<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: difekt.xml,v 1.28 2021/05/29 10:56:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>difekt</rad>/i
</kap>
<drv mrk="difekt.0i">
   <kap><ofc>*</ofc><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc mrk="difekt.0i.KOMUNE">
     <dif>
       Ka&ubreve;zi malbonan staton de io, tiel ke &gcirc;i i&gcirc;as
       malpli ta&ubreve;ga, malpli valora:
       <ekz>
         <tld/>i vazon, meblon, ma&scirc;inon;
       </ekz>
       <ekz>
         <tld/>i al si la brakon;
       </ekz>
       <ekz>
         unu ovo malbona <klr>(putrinta)</klr> tutan man&gcirc;on
         <tld/>as
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         <tld/>i la sanon, virton<fnt>Z</fnt>,
         &gcirc;ojon<fnt>Z</fnt>, honoron<fnt>Z</fnt>,
         famon, rajtojn de iu<fnt>Z</fnt>;
       </ekz>
       <ekz>
         unu malamiko pli <tld/>as ol cent amikoj protektas
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         <klr>[la kamentubo]</klr> <tld/>as la pejza&gcirc;on,
         el&jcirc;etante nigran fumon en la bluan &ccirc;ielon
         <fnt><bib>Lanti</bib> <lok>La kamentubo</lok></fnt>;
       </ekz>
       <ekz>
         la hajlo <ind>bat<tld/>i</ind>s en la tuta lando Egipta
         &ccirc;ion, kio estis sur la kampo
         <fnt><bib>MT</bib> <lok>&Eli; 9:25</lok></fnt>.
         <trd lng="fr">frapper<klr tip="ind"> (en
         ab&icirc;mant)</klr></trd>
         <trd lng="hu">elver<klr tip="ind"> (term&eacute;st)</klr></trd>
       </ekz>
     </dif>
     <ref tip="ant" cel="ripar.0i">ripari</ref>
     <refgrp tip="vid">
       <ref cel="brul.0difekti">bruldifekti</ref>,
       <ref cel="fusx.0i">fu&scirc;i</ref>,
       <ref cel="konfuz.0i">konfuzi</ref>,
       <ref cel="kripl.0igi">kripligi</ref>,
       <ref cel="util.mal0i">malutili</ref>,
       <ref cel="romp.0i">rompi</ref>,
       <ref cel="fos.sub0i">subfosi</ref>
     </refgrp>
   </snc>
  <trdgrp lng="be">
     <trd>&c_p;&c_a;&c_sh;&c_k;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>,
     <trd>&c_s;&c_a;&c_p;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_p;&c_a;&c_p;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_p;&c_a;&c_l;&c_a;&c_m;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_z;&c_l;&c_a;&c_m;&c_a;&c_c;&c_mol;</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>pokazit</trd>,
    <trd>porouchat</trd>,
    <trd>po&scaron;kodit</trd>,
    <trd>rozru&scaron;it</trd>,
    <trd>ubl&iacute;&zcaron;it</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>besch&auml;digen</trd>,
     <trd>kaputt machen</trd>
   </trdgrp>
  <trdgrp lng="en">
     <trd>damage</trd>,
     <trd>impair</trd>,
     <trd>mar</trd>,
     <trd>spoil</trd>
   </trdgrp>
  <trdgrp lng="fr">
     <trd>ab&icirc;mer<klr tip="ind">
     (d&eacute;t&eacute;riorer)</klr></trd>,
     <trd>d&eacute;t&eacute;riorer</trd>,
     <trd>endommager</trd>
   </trdgrp>
  <trd lng="he">&#x5DC;&#x5E7;&#x5DC;&#x5E7;&#x5DC;</trd>
  <trdgrp lng="hu">
     <trd>megrong&aacute;l</trd>,
     <trd>elront</trd>,
     <trd>meghib&aacute;s&iacute;t</trd>
   </trdgrp>
  <trdgrp lng="pl">
     <trd>psu&cacute;</trd>,
     <trd>uszkadza&cacute;</trd>
   </trdgrp>
  <trdgrp lng="pt">
    <trd>danificar</trd>,
    <trd>avariar</trd>,
    <trd>estragar</trd>,
    <trd>deteriorar</trd>,
    <trd>arruinar</trd>
  </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_i;&c_s;&c_p;&c_o;&c_r;&c_t;&c_i;&c_t;&c_mol;</trd>,
     <trd>&c_p;&c_o;&c_v;&c_r;&c_je;&c_d;&c_i;&c_t;&c_mol;</trd>,
     <trd>&c_p;&c_o;&c_l;&c_o;&c_m;&c_a;&c_t;&c_mol;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>pobabra&tcaron;</trd>,
    <trd>pokazi&tcaron;</trd>,
    <trd>po&scaron;kodi&tcaron;</trd>
  </trdgrp>
   <trdgrp lng="th">
     <trd>&#xE17;&#xE33;&#xE43;&#xE2B;&#xE49;&#xE44;&#xE21;&#xE48;&#xE14;&#xE35;</trd>,
     <trd>&#xE17;&#xE33;&#xE43;&#xE2B;&#xE49;&#xE40;&#xE2A;&#xE35;&#xE22;&#xE2B;&#xE32;&#xE22;</trd>
   </trdgrp>
</drv>

<drv mrk="difekt.0o">
   <kap><tld/>o</kap>
   <snc mrk="difekt.0o.KOMUNE">
     <dif>
       Malbona stato de io, kio estas <tld/>ita; manko,
       malperfekta&jcirc;o:
       <ekz>
         en &ccirc;iu objekto trovi&gcirc;as <tld/>o
         <fnt><bib>PrV</bib></fnt>
         (<ref tip="vid" cel="flank.0o">flanko 2</ref>);
       </ekz>
       <ekz>
         <tld/>oj de domo kaduka<fnt>Z</fnt>;
       </ekz>
       <ekz>
         la forigo de akuzativo estus granda <tld/>o por la
         fleksebleco, ri&ccirc;eco kaj precizeco de la
         lingvo<fnt>Z</fnt>;
       </ekz>
       <ekz>
         fari <tld/>on al sia proksimulo
         <fnt><bib>MT</bib> <lok>&Lev; 24:19</lok></fnt>;
       </ekz>
       <ekz>
         &scirc;i falis, sed faris al si nenian <tld/>on<fnt>Z</fnt>;
       </ekz>
       <ekz>
         lia karaktero havas grandan <tld/>on;
       </ekz>
       <ekz>
         neniun beston, kiu havas <tld/>a&jcirc;on, alportu, &ccirc;ar
         &gcirc;i ne akirigos al vi pla&ccirc;on
         <fnt><bib>MT</bib> <lok>&Lev; 22:20</lok></fnt>.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="averi.0o">averio</ref>,
       <ref cel="kripl.0ajxo">kripla&jcirc;o</ref>,
       <ref cel="makul.0o">makulo</ref>,
       <ref cel="tuber.0o">tubero</ref>
     </refgrp>
   </snc>
  <trdgrp lng="be">
     <trd>&c_sh;&c_k;&c_o;&c_d;&c_a;</trd>,
     <trd>&c_p;&c_a;&c_sh;&c_k;&c_o;&c_d;&c_zh;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
     <trd>&c_p;&c_a;&c_l;&c_o;&c_m;&c_k;&c_a;</trd>,
     <trd>&c_d;&c_e;&c_f;&c_e;&c_k;&c_t;</trd>
   </trdgrp>
  <trd lng="bo">&#xF66;&#xF90;&#xFB1;&#xF7C;&#xF53;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>defekt</trd>,
    <trd>kaz</trd>,
    <trd>porucha</trd>,
    <trd>poru&scaron;en&iacute;</trd>,
    <trd>vada</trd>,
    <trd>z&aacute;vada</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Schaden</trd>,
     <trd>Besch&auml;digung</trd>,
     <trd>Defekt</trd>
   </trdgrp>
  <trdgrp lng="en">
     <trd>defect</trd>,
     <trd>flaw</trd>,
     <trd>imperfection</trd>
   </trdgrp>
  <trd lng="es">defecto</trd>
  <trdgrp lng="fr">
     <trd>d&eacute;faut</trd>,
     <trd>d&eacute;fectuosit&eacute;</trd>,
     <trd>dommage</trd>,
     <trd>vice<klr tip="ind"> (d&eacute;faut)</klr></trd>
   </trdgrp>
  <trdgrp lng="he">
    <trd>&#x5E4;&#x5BC;&#x5B0;&#x5D2;&#x5B8;&#x5DD;</trd>,
    <trd>&#x5E7;&#x5DC;&#x5E7;&#x5D5;&#x5DC;</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>hiba</trd>,
     <trd>k&aacute;r</trd>,
     <trd>s&eacute;r&uuml;l&eacute;s</trd>
   </trdgrp>
  <trdgrp lng="pl">
     <trd>defekt</trd>,
     <trd>usterka</trd>,
     <trd>uszkodzenie</trd>,
     <trd>skaza</trd>,
     <trd>wada</trd>
   </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_p;&c_o;&c_l;&c_o;&c_m;&c_k;&c_a;</trd>,
     <trd>&c_p;&c_o;&c_v;&c_r;&c_je;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>,
     <trd>&c_d;&c_je;&c_f;&c_je;&c_k;&c_t;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>defekt</trd>,
    <trd>kaz</trd>,
    <trd>porucha</trd>,
    <trd>po&scaron;kodenie</trd>,
    <trd>z&aacute;vada</trd>
  </trdgrp>
</drv>

<drv mrk="difekt.0ebla">
  <kap><tld/>ebla</kap>
  <snc mrk="difekt.0ebla.KOMUNE">
    <dif>
      Kiu povas ricevi <tld/>on,
      <ref tip="vid" cel="fortik.mal0a.KOMUNE">malfortika</ref>:
      <ekz>
        facile <tld/>ebla aparato;
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">fragile</trd>
  <trd lng="hu">elronthat&oacute;</trd>
</drv>

<drv mrk="difekt.0igxi">
   <kap><tld/>i&gcirc;i</kap>
   <snc mrk="difekt.0igxi.KOMUNE">
     <dif>
       Fari&gcirc;i <tld/>ita:
       <ekz>
         mia pluvombrelo <tld/>i&gcirc;is de vento;
       </ekz>
       <ekz>
         nenie rompi&gcirc;as a&ubreve; <tld/>i&gcirc;as la kontinueco
         inter la lingvo malnova kaj nova<fnt>Z</fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_p;&c_a;&c_sh;&c_k;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_c;&c_a;</trd>,
     <trd>&c_s;&c_a;&c_p;&c_s;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
     <trd>&c_p;&c_a;&c_p;&c_s;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
     <trd>&c_p;&c_a;&c_l;&c_a;&c_m;&c_a;&c_c;&c_c;&c_a;</trd>,
     <trd>&c_z;&c_l;&c_a;&c_m;&c_a;&c_c;&c_c;&c_a;</trd>
   </trdgrp>
   <trd lng="de">kaputt gehen</trd>
   <trd lng="en">deteriorate</trd>
   <trdgrp lng="fr">
     <trd>s'<ind>abimer</ind><klr tip="ind">
     (se d&eacute;t&eacute;riorer)</klr></trd>,
     <trd>se <ind>d&eacute;t&eacute;riorer</ind></trd>,
     <trd>s'<ind>endommager</ind></trd>
   </trdgrp>
   <trd lng="he">&#x5DC;&#x5D4;&#x5EA;&#x5E7;&#x5DC;&#x5E7;&#x5DC;</trd>
  <trdgrp lng="pl">
     <trd>psu&cacute; si&eogonek;</trd>,
     <trd>uszkadza&cacute; si&eogonek;</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_i;&c_s;&c_p;&c_o;&c_r;&c_t;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
     <trd>&c_p;&c_o;&c_v;&c_r;&c_je;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
     <trd>&c_p;&c_o;&c_l;&c_o;&c_m;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
   </trdgrp>
</drv>

<drv mrk="difekt.0igxema">
   <kap><tld/>i&gcirc;ema</kap>
   <snc mrk="difekt.0igxema.KOMUNE">
     <dif>
       Facile <tld/>i&gcirc;anta a&ubreve; <tld/>ebla;
       delikata 3.
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_l;&c_o;&c_m;&c_k;&c_ib;</trd>,
     <trd>&c_k;&c_r;&c_o;&c_h;&c_k;&c_ib;</trd>
   </trdgrp>
   <trd lng="en">perishable</trd>
   <trdgrp lng="fr">
     <trd>fragile</trd>,
     <trd>p&eacute;rissable</trd>
   </trdgrp>
   <trdgrp lng="he">
    <trd>&#x5E2;&#x5D3;&#x5D9;&#x5DF;</trd>,
    <trd>&#x5E0;&#x5D5;&#x5D8;&#x5D4; &#x5DC;&#x5D4;&#x5EA;&#x5E7;&#x5DC;&#x5E7;&#x5DC;</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_o;&c_m;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="difekt.0ita">
  <kap><tld/>ita</kap>
  <snc mrk="difekt.0ita.KOMUNE">
    <dif>
      Trafita de manko, de malbonigo:
      <ekz>
        longan tempon oni vidadis <tld/>itajn arbojn
        sur la kampo kaj duone detruitajn barilojn kaj
        murojn sur la lokoj, kie estis mortiga mana
        batalado
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        multaj rompitaj kolonoj, <tld/>aj statuoj kaj multe
        da aliaj ruinoj elokvente konvinkas <klr>[pri la
        supereco]</klr> de la antikvaj Romanoj
        <fnt>
          <bib>Lanti</bib>
          <lok>&Ccirc;e la lando de la nigra&ccirc;emizuloj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>abim&eacute;</trd>,
    <trd>d&eacute;fectueux</trd>,
    <trd>endommag&eacute;</trd>
  </trdgrp>
  <trd lng="hu">s&eacute;r&uuml;lt</trd>
</drv>

<drv mrk="difekt.ne0ebla">
  <kap>ne<tld/>ebla</kap>
  <snc mrk="difekt.ne0ebla.KOMUNE">
    <dif>
      Kiun nenio povas <tld/>i, vundi, pereigi:
      <ekz>
        lia gloro estas ne<tld/>ebla<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>inalt&eacute;rable</trd>,
    <trd>ind&eacute;fectible</trd>,
    <trd>imp&eacute;rissable</trd>
  </trdgrp>
</drv>

<drv mrk="difekt.sen0a">
   <kap>sen<tld/>a</kap>
   <snc mrk="difekt.sen0a.KOMUNE">
     <dif>
       Ne<tld/>ita
     </dif>
     <refgrp tip="vid">
       <ref cel="bon">bon(stat)a</ref>,
       <ref cel="tusx">netu&scirc;ita</ref>,
       <ref cel="tauxg.0a">ta&ubreve;ga</ref>,
       <ref cel="virg.0a">virga</ref>
     </refgrp>
   </snc>
  <trdgrp lng="be">
     <trd>&c_c;&c_e;&c_l;&c_y;</trd>,
     <trd>&c_n;&c_je;&c_p;&c_a;&c_sh;&c_k;&c_o;&c_d;&c_zh;&c_a;&c_n;&c_y;</trd>,
     <trd>&c_s;&c_p;&c_r;&c_a;&c_w;&c_n;&c_y;</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>bezvadn&yacute;</trd>,
    <trd>cel&yacute;</trd>,
    <trd>neporu&scaron;en&yacute;</trd>,
    <trd>nepo&scaron;kozen&yacute;</trd>,
    <trd>nez&aacute;vadn&yacute;</trd>,
    <trd>zachoval&yacute;</trd>
  </trdgrp>
  <trd lng="de">ganz</trd>
  <trd lng="en">flawless</trd>
  <trdgrp lng="fr">
     <trd>impeccable<klr tip="ind"> (sans d&eacute;faut)</klr></trd>,
     <trd>intact</trd>,
     <trd>sans <ind>d&eacute;faut</ind></trd>
   </trdgrp>
  <trdgrp lng="pl">
     <trd>nieuszkodzony</trd>,
     <trd>nienaruszony</trd>,
     <trd>ca&lstroke;y</trd>,
     <trd>sprawny</trd>,
     <trd>bez <ind>wady</ind></trd>,
     <trd>bez <ind>skazy</ind></trd>
   </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_c;&c_je;&c_l;&c_y;&c_j;</trd>,
     <trd>&c_i;&c_s;&c_p;&c_r;&c_a;&c_v;&c_n;&c_y;&c_j;</trd>,
     <trd>&c_n;&c_je;&c_p;&c_o;&c_v;&c_r;&c_je;&c_zh;&c_d;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>,
     <trd>&c_n;&c_je;&c_v;&c_r;&c_je;&c_d;&c_i;&c_m;&c_y;&c_j;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>bezchybn&yacute;</trd>,
    <trd>cel&yacute;</trd>,
    <trd>neporu&scaron;en&yacute;</trd>,
    <trd>nepo&scaron;koden&yacute;</trd>
  </trdgrp>
</drv>
</art>
