<kap>
  <ofc>8</ofc> <rad>nilon</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="nilon.0o">
  <kap><tld/>o</kap>
  <snc mrk="nilon.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Tre solida sinteza fibro poliamida, uzata por &scirc;nuroj, teksita
      kiel
      <ref tip="super" cel="sxtof.0o.TEKS">&scirc;tofo</ref> por vestoj...:
      <ekz>
        modernaj kajtoj ofte fari&gcirc;as el sintezaj materialoj, kiel
        <tld/>o
        por la veloj
        <fnt>
        <bib>Viki</bib>,
        <lok>kajto, 2005-07-17</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_e;&c_j;&c_l;&c_o;&c_n;</trd>,
    <trd>&c_n;&c_a;&c_j;&c_l;&c_o;&c_n;</trd>,
    <trd>&c_k;&c_a;&c_p;&c_r;&c_o;&c_n;</trd>
  </trdgrp>
  <trd lng="bg">&c_n;&c_a;&c_j;&c_l;&c_o;&c_n;</trd>
  <trd lng="cs">nylon</trd>
  <trd lng="de">Nylon</trd>
  <trd lng="en">nylon</trd>
  <trd lng="es">nailon, nil&oacute;n</trd>
  <trd lng="fa">&a_n;&a_A;&f_y;&a_l;&a_w;&a_n;</trd>
  <trd lng="fr">nylon</trd>
  <trd lng="ja">&#x30CA;&#x30A4;&#x30ED;&#x30F3;</trd>
  <trd lng="nl">nylon</trd>
  <trd lng="pl">nylon</trd>
  <trdgrp lng="pt">
    <trd>nylon</trd>,
    <trd>n&aacute;ilon</trd>    
  </trdgrp>
  <trd lng="ro">nailon</trd>
  <trd lng="ru">&c_n;&c_je;&c_j;&c_l;&c_o;&c_n;</trd>
  <trd lng="sk">nylon</trd>
  <trd lng="uk">&#x43D;&#x435;&#x439;&#x43B;&#x43E;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x5C3C;&#x9F8D; <pr>n&#xED;l&#xF3;ng</pr></trd>,
    <trd>&#x5C3C;&#x9F99; <pr>n&#xED;l&#xF3;ng</pr></trd>,
    <trd>&#x8010;&#x7DB8; <pr>n&#xE0;il&#xFA;n</pr></trd>,
    <trd>&#x8010;&#x7EB6; <pr>n&#xE0;il&#xFA;n</pr></trd>,
    <trd>&#x5C3C;&#x9F8D;&#x7E54;&#x7269; <pr>n&#xED;l&#xF3;ngzh&#x12B;w&#xF9;</pr></trd>,
    <trd>&#x5C3C;&#x9F99;&#x7EC7;&#x7269; <pr>n&#xED;l&#xF3;ngzh&#x12B;w&#xF9;</pr></trd>
  </trdgrp>
</drv>
