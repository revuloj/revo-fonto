<art mrk="$Id$">
<kap>
  <rad>nilon</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="nilon.0o">
  <kap><tld/>o</kap>
  <snc mrk="nilon.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Tre solida sinteza fibro poliamida, uzata por &scirc;nuroj, teksita kiel
      <ref tip="super" cel="sxtof.0o.TEKS">&scirc;tofo</ref> por vestoj...:
      <ekz>
        modernaj kajtoj ofte fari&gcirc;as el sintezaj materialoj, kiel <tld/>o
        por la veloj
        <fnt>
        <bib>Viki</bib>,
        <lok>kajto, 2005-07-17</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">nylon</trd>
  <trd lng="ru">&c_n;&c_je;&c_j;&c_l;&c_o;&c_n;</trd>
</drv>
</art>

