<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>komuni</rad>/o
</kap>
<drv mrk="komuni.0o">
<kap><ofc>*</ofc><tld/>o</kap>
<uzo tip="fak">KRI</uzo>
<snc>
<dif>
Partopreno al sakramento de E&ubreve;karistio. 
</dif>
</snc>
<trd lng="nl">communie</trd>
</drv>
<drv mrk="komuni.0i">
<kap><tld/>i</kap>
<gra><vspec>tr</vspec></gra>
<snc>
<dif>
Doni la <tld/>on. 
</dif>
</snc>
<trd lng="nl">communie uitreiken</trd>
</drv>
<drv mrk="komuni.0igxi">
<kap><tld/>i&gcirc;i</kap>
<snc>
<dif>
Ricevi la <tld/>on. 
</dif>
</snc>
<trd lng="nl">communie ontvangen</trd>
</drv>
<drv mrk="komuni.for0igi">
<kap>for<tld/>igi</kap>
<snc>
<dif>
Ekskomuniki.  
</dif>
</snc>
<trdgrp lng="nl"><trd>excommuniceren</trd>,
  <trd>in de <ind>ban</ind> doen</trd></trdgrp>
</drv>
</art>
