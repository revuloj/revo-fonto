<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: komuni.xml,v 1.19 2019/03/18 18:10:20 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>komuni</rad>/i
</kap>
<drv mrk="komuni.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Doni la <tld/>on:
      <ekz>
        la pastro <tld/>as la absolvitan pekulon
        <fnt><bib>VdE</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">combregar <klr>(tr.)</klr></trd>
  <trd lng="cs">pod&aacute;vat sv&aacute;tost olt&aacute;&rcaron;n&iacute;</trd>
  <trdgrp lng="de">
    <trd>die (<ind>Heilige) Kommunion</ind> verteilen</trd>,
    <trd>das Abendmahl reichen</trd>
  </trdgrp>
  <trd lng="en">administer Holy <ind>Communion</ind></trd>
  <trd lng="es">dar la <ind>comuni&oacute;n</ind></trd>
  <trd lng="fr">donner la <ind>communion</ind></trd>
  <trd lng="hu">(meg)<ind>&aacute;ldoztat</ind></trd>
  <trd lng="nl">communie uitreiken</trd>
  <trd lng="pl">udziela&cacute; <ind>komunii</ind></trd>
  <trdgrp lng="pt">
    <trd>dar a <ind>comunh&atilde;o</ind></trd>,
    <trd>comungar</trd>,    
    <trd>administrar o <ind>sacramento</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">da&tcaron; prij&iacute;manie</trd>
</drv>

<drv mrk="komuni.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KRI</uzo>
  <snc>
    <dif>
      Partopreno al sakramento de
      <ref tip="vid" cel="euxhxaristi.0o">E&ubreve;&hcirc;aristio</ref>.
    </dif>
  </snc>
  <adm>
    La redaktoj de VdE, PV/PIV1 kaj PIV2 ne kongruas. Entute
    la afero estas nebula. La radika karaktero neprigas, ke
    ~o havu la sencon "dono" (sed cxu "dono de ~o"??). La PIV2-a
    redakto estas cxiokapta. Kp kun "~igxo". Franca traduko nur
    provizora.
    <aut>MB</aut>
  </adm>
  <trd lng="ca">comuni&oacute;</trd>
  <trdgrp lng="de">
    <trd>Kommunion</trd>,
    <trd>Heilige Kommunion</trd>,
    <trd>Abendmahl</trd>,
    <trd>Heiliges Abendmahl</trd>,
    <trd>Eucharistiefeier</trd>
  </trdgrp>
  <trd lng="es">comuni&oacute;n</trd>
  <trd lng="fr">communion<klr tip="ind"> (religieuse)</klr></trd>
  <trdgrp lng="hu">
    <trd>&aacute;ldoztat&aacute;s</trd>,
    <trd>szent&aacute;ldoz&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">communie</trd>
  <trdgrp lng="pl">
    <trd>komunia</trd>,
    <trd>naj&sacute;wi&eogonek;tszy sakrament</trd>,
    <trd>eucharystia</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>eucharistia</trd>,
    <trd>sviatos&tcaron; olt&aacute;rna</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>eucharistie</trd>,
    <trd>sv. p&rcaron;&iacute;j&iacute;m&aacute;n&iacute;</trd>,
    <trd>sv&aacute;tost olt&aacute;&rcaron;n&iacute;</trd>
  </trdgrp>
</drv>

<drv mrk="komuni.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Ricevi la <tld/>on.
    </dif>
  </snc>
  <trd lng="ca">combregar <klr>(intr.)</klr></trd>
  <trdgrp lng="de">
    <trd>an der (<ind>Heiligen) Kommunion</ind> teilnehmen</trd>,
    <trd>das Abendmahl feiern</trd>
  </trdgrp>
  <trd lng="en">receive Holy <ind>Communion</ind></trd>
  <trd lng="es">comulgar</trd>
  <trd lng="fr">communier<klr tip="ind"> (rel.)</klr></trd>
  <trdgrp lng="hu">
    <trd>&aacute;ldozik</trd>,
    <trd>szent&aacute;ldoz&aacute;shoz j&aacute;rul</trd>
  </trdgrp>
  <trd lng="nl">communie ontvangen</trd>
  <trd lng="pl">przyst&eogonek;powa&cacute; do <ind>komunii</ind></trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="komuni.for0igi">
  <kap>for<tld/>igi</kap>
  <snc>
    <ref tip="dif" cel="ekskom.0i">ekskomuniki</ref>
  </snc>
  <trd lng="ca">excomunicar</trd>
  <trdgrp lng="de">
    <trd>exkommunizieren</trd>,
    <trd>aus der kirchlichen Gemeinschaft
    <ind>ausschlie&szlig;en</ind></trd>
  </trdgrp>
  <trd lng="en">excommunicate</trd>
  <trd lng="es">excomulgar</trd>
  <trd lng="fr">excommunier</trd>
  <trd lng="hu">kik&ouml;z&ouml;s&iacute;t</trd>  
  <trdgrp lng="nl">
    <trd>excommuniceren</trd>,
    <trd>in de <ind>ban</ind> doen</trd>
  </trdgrp>
  <trd lng="pl">ekskomunikowa&cacute;</trd>
  <trd lng="ru">&c_o;&c_t;&c_l;&c_u;&c_ch;&c_i;&c_t;&c_mol;
  <klr>(&c_o;&c_t; &c_c;&c_je;&c_r;&c_k;&c_v;&c_i;)</klr></trd>
</drv>
</art>
