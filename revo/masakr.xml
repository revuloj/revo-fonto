  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_t;&c_r;&c_je;&c_b;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol; &c_m;&c_a;&c_s;&c_s;&c_o;&c_v;&c_o;&c_je; &c_u;&c_n;&c_i;&c_ch;&c_t;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
