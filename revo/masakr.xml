<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: masakr.xml,v 1.17 2019/06/30 07:42:48 revo Exp $">
<kap>
  <rad>masakr</rad>/i
</kap>
<drv mrk="masakr.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Amasbu&ccirc;i.
    </dif>
    <ekz>
      Masakritaj estas 158 homoj &ccirc;e rifu&gcirc;ejo apud la
      landlimo inter Burundo kaj Demokratia Respubliko Kongo (DRK).
      <fnt>
        <aut>SABIYUMVA J&eacute;r&eacute;mie</aut>,
        <vrk>
          <url ref="https://www.monato.be/2004/008413.html">
          Masakro &ccirc;e rifu&gcirc;ejo</url>
        </vrk>,
        <lok>Monato, septembro 2004</lok>
     </fnt>
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_n;&c_ib;&c_sh;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_r;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_b;&c_ib;&c_v;&c_a;&c_c;&c_mol;
    <klr>(&c_m;&c_a;&c_s;&c_a;&c_v;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="ca">massacrar</trd>
  <trdgrp lng="de">
    <trd>massakrieren</trd>,
    <trd>niedermetzeln</trd>,
    <trd>hinmorden</trd>,
    <trd>massenhaft <ind>umbringen</ind></trd>,
    <trd>hinschlachten</trd>,
    <trd>niedermetzeln</trd>
  </trdgrp>
  <trd lng="fr">massacrer</trd>
  <trd lng="hu">lem&eacute;sz&aacute;rol
  <klr tip="amb">(t&ouml;megesen)</klr></trd>
  <trd lng="tr">katletmek</trd>  
  <trdgrp lng="nl">
    <trd>afslachten</trd>,
    <trd>moorden</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>masakrowa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">massacrar</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_t;&c_r;&c_je;&c_b;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol;</trd>,
    <trd>
    &c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol;
    &c_m;&c_a;&c_s;&c_s;&c_o;&c_v;&c_o;&c_je;
    &c_u;&c_n;&c_i;&c_ch;&c_t;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;
    </trd>
  </trdgrp>
</drv>

<drv mrk="masakr.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Murdo de homoj a&ubreve; bestoj en precipe kruelaj cirkonstancoj.
      Amasmurdo, amasbu&ccirc;ado.
	    <ekz>
           La masakro estis reprezalio pro la agado de la partizano
           Stella Rossa (ru&gcirc;a stelo). La krima masakro dum multaj
           jaroj stre&ccirc;is la rilatojn inter Italio kaj Germanio.
	      <fnt>
	        <vrk>
  <url ref="http://sociala.respubliko.itala.eo.ezlemonpierecipes.info/">
	        Masakro de Marzabotto</url>
	        </vrk>
	     </fnt>
	    </ekz>:
    </dif>
    <trd lng="ca">massacre</trd>
    <trd lng="fr">massacre</trd>
    <trdgrp lng="pl">
      <trd>masakra</trd>,
      <trd>rze&zacute;</trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>m&abreve;cel</trd>,
      <trd>masacru</trd>
    </trdgrp>
    <trd lng="tr">katliyam</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>hekatomba</trd>,
    <trd>krveprolit&iacute;</trd>,
    <trd>masakr</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hromadn&eacute; vra&zcaron;denie</trd>,
    <trd>masakra</trd>
  </trdgrp>
</drv>
</art>
