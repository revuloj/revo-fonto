<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>zoologi</rad>/o
<fnt><bib>F</bib>, <lok>Ekzercaro, &para; 4</lok></fnt>  
</kap>
<drv mrk="zoolog1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="scienc.0o.fako">Scienco</ref> pri
      la <ref tip="prt" cel="best.0oj">bestoj</ref>, subfako
      de la <ref tip="super" cel="biolog1.0o">biologio</ref>:
      <ekz>
        <tld/>a bestarejo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Zoologie</trd>
  <trd lng="es">zoolog&iacute;a</trd>
  <trd lng="nl">zoologie</trd>
  <trd lng="ru">&c_z;&c_o;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trdgrp lng="sv">
    <trd>zoologi</trd>,
    <trd>djurl&auml;ra</trd>
  </trdgrp>
</drv>
<drv mrk="zoolog1.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Tiu, kiu okupas sin pri
      <ref tip="malprt" cel="zoolog1.0o"><tld/>o</ref>.
    </dif>
  </snc>
  <trd lng="de">Zoologe</trd>
  <trd lng="nl">zooloog</trd>
  <trd lng="sv">zoolog</trd>
</drv>
</art>
