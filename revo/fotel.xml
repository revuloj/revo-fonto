<kap>
  <ofc>8</ofc>
  <rad>fotel</rad>/o <fnt> <bib>PV</bib></fnt>
</kap>
<drv mrk="fotel.0o">
  <kap><tld/>o</kap>
  <snc mrk="fotel.0o.MEBL">
    <dif>
      Komforta, ordinare malalta, remburita
      <ref tip="lst" lst="voko:mebloj" cel="segx.apog0o.MEBL">brakse&gcirc;o</ref>:
      <ekz>
        risortita <tld/>o
        <fnt>
          <bib>VaK</bib>
          <lok>Dektria &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la partoprenantoj dissidi&gcirc;is en pezaj klub<tld/>oj
          <fnt>
            <aut>S. Bubeni&ccaron;</aut>
            <vrk>Sub Cirkotendo</vrk>
            <lok>1969</lok>
          </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo> ministroj perdas siajn komfortajn <tld/>ojn
        <fnt>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2000/007252.php">Du mil</url></vrk>
          <bib>Monato</bib>
          <lok>2000/12, p. 5</lok>
        </fnt>;
      </ekz>
      <ekz>
        Koriandro sidis <klr>[...]</klr> en sia eluzita led<tld/>o
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Akvo de l' Vivo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Anka&ubreve; eblas diri <ke>brakse&gcirc;o</ke>.
  </rim>
  <trd lng="be">&c_f;&c_a;&c_t;&c_e;&c_l;&c_mol;</trd>
  <trd lng="bg">&c_f;&c_o;&c_t;&c_mol;&c_o;&c_j;&c_l;</trd>
  <trdgrp lng="ca">
    <trd>cadira de bra&ccedil;os</trd>,
    <trd>butaca</trd>,
    <trd>poltrona</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>fotel</trd>,
    <trd>k&rcaron;eslo</trd>,
    <trd>leno&scaron;ka</trd>
  </trdgrp>
  <trd lng="de">Sessel</trd>
  <trdgrp lng="en">
    <trd>easy-chair</trd>,
    <trd>armchair</trd>
  </trdgrp>
  <trd lng="es">sill&oacute;n</trd>
  <trd lng="fr">fauteuil</trd>
  <trd lng="hu">fotel</trd>
  <trd lng="ja">&#x8098;&#x639B;&#x3051;&#x6905;&#x5B50;  <pr>&#x3072;&#x3058;&#x304B;&#x3051;&#x3044;&#x3059;</pr></trd>
  <trdgrp lng="nl">
    <trd>fauteuil</trd>,
    <trd>zetel</trd>
  </trdgrp>
  <trd lng="pl">fotel</trd>
  <trd lng="pt">poltrona</trd>
  <trd lng="ro">foltoliu</trd>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_s;&c_l;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>fotelka</trd>,
    <trd>kreslo</trd>
  </trdgrp>
  <trd lng="sv">f&aring;t&ouml;lj</trd>
  <trd lng="uk">&c_k;&c_r;&c_ib;&c_s;&c_l;&c_o;</trd>
  <trdgrp lng="zh">
    <trd>&#x6C99;&#x53D1;&#x6905; <pr>sh&amacron;f&amacron;y&#x1D0;</pr></trd>,
    <trd>&#x5355;&#x4EBA;&#x6C99;&#x53D1; <pr>d&amacron;nr&eacute;nsh&amacron;f&amacron;</pr></trd>
