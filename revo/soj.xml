<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>soj</rad>/o
</kap>
<drv mrk="soj.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="glicin1.0o">glicineo</ref>
      (<trd lng="la">Glycine soja</trd>), grava legomo, el kies
      faboj oni produktas oleon, farunon k.a.:
      <ekz>
        <tld/>oleo, <tld/>faruno, <tld/>sa&ubreve;co.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Soja</trd>
</drv>
</art>

