<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gutape.xml,v 1.22 2023/11/11 11:36:37 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>gutaperk</rad>/o
</kap>
<drv mrk="gutape.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Rezina <ref tip="super" cel="gum.0o">gumo</ref>,
      simila al ka&ubreve;&ccirc;uko, kaj pasintece uzata por tegi
      elektrajn kablojn:
      <ekz>
        <nom>Werner von Siemens</nom> <klr>[&#x2026;]</klr> kreis sistemon de izolado de elektraj kabloj
        per <tld/>o, kiu e&ccirc; permesis la konstruon de submaraj kabloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=376028">Werner von Siemens</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_t;&c_a;&c_p;&c_e;&c_r;&c_ch;&c_a;</trd>
  <trd lng="ca">gutaperxa</trd>
  <trd lng="cs">gutaper&ccaron;a</trd>
  <trd lng="de">Guttapercha</trd>
  <trd lng="en">gutta-percha</trd>
  <trd lng="es">gutapercha</trd>
  <trd lng="fa">&f_g;&a_w;&a_t;&a_A;&f_p;&a_r;&f_ch;&a_A;</trd>
  <trd lng="fr">gutta-percha</trd>
  <trd lng="hu">guttapercha</trd>
  <trd lng="ja">&#x30B0;&#x30C3;&#x30BF;&#x30DA;&#x30EB;&#x30AB;</trd>
  <trd lng="nl">guttapercha</trd>
  <trd lng="pl">gutaperka</trd>
  <trdgrp lng="pt">
    <trd>guta-percha</trd>,
    <trd>guta</trd>,
    <trd>percha</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_u;&c_t;&c_t;&c_a;&c_p;&c_je;&c_r;&c_ch;&c_a;</trd>
  <trd lng="sk">gutaper&ccaron;a</trd>
  <trd lng="uk">&#x491;&#x443;&#x442;&#x430;&#x43F;&#x435;&#x440;&#x447;&#x430;</trd>
  <trd lng="zh">&#x53E4;&#x5854;&#x6CE2;&#x80F6; <pr>g&#x1D4;t&#x1CE;b&#x14D;ji&#x101;o</pr></trd>
</drv>
</art>
