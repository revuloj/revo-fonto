<vortaro>
<art mrk="$Id: endoskopi.xml,v 1.13 2022/03/17 13:47:18 revo Exp $">
<kap>
  <rad>endoskopi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="endoskopi.0o">
  <kap><tld/>o</kap>
  <snc mrk="endoskopi.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Medicina ekzamenado de korpaj kavoj per <ref cel="endosk.0o">endoskopo</ref>:
      <ekz>
        speciali&gcirc;oj ene de la subfako <klr>[gastroenterologio]</klr> estas la <tld/>o,
        la hepatologio, la proktologio, kaj la gastroenterologia onkologio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=269145">Gastroenterologio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_n;&c_d;&c_a;&c_s;&c_k;&c_a;&c_p;&c_ib;&c_ja;</trd>
  <trd lng="cs">endoskopie</trd>
  <trd lng="de">Endoskopie</trd>
  <trd lng="en">endoscopy</trd>
  <trd lng="es">endoscopia</trd>
  <trdgrp lng="fa">
    <trd>&a_d;&a_r;&a_w;&a_n;&zwnj;&a_b;&f_y;&a_n;&f_y;</trd>,
    <trd>&a_A_madda;&a_n;&a_d;&a_w;&a_s;&f_k;&f_p;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">endoscopie</trd>
  <trd lng="pl">endoskopia</trd>
  <trd lng="pt">endoscopia</trd>
  <trd lng="ru">&c_e;&c_n;&c_d;&c_o;&c_s;&c_k;&c_o;&c_p;&c_i;&c_ja;</trd>
  <trd lng="sk">endoskopia</trd>
  <trd lng="uk">&#x435;&#x43D;&#x434;&#x43E;&#x441;&#x43A;&#x43E;&#x43F;&#x456;&#x44F;</trd>
</drv>
</art>
