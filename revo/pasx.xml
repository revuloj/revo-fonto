<drv mrk="pasx.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="pasx.el0i.eliri">
    <dif>
      Eliri el la vico, anta&ubreve;eniri:
      <ekz>
        <nom>Edom</nom> el<tld/>is kontra&ubreve; lin kun popolo multenombra
        <fnt>
          <bib>MT</bib>
          <lok>&Nom; 20:20</lok>
        </fnt>;
      </ekz><ekz>
        el<tld/>u la pastro kaj parolu al la popolo
        <fnt>
          <bib>MT</bib>
          <lok>&Rea; 20:2</lok>
        </fnt>;
      </ekz><ekz>
        el amba&ubreve; veturiloj el<tld/>is po kvar viroj
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro 16a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">heraus <ind>treten</ind></trd>
    <trd lng="hu">kil&eacute;p</trd>
    <trdgrp lng="id">
      <trd><baz>hadap</baz>menghadapi</trd>,
      <trd><baz>luar</baz>keluar barisan</trd>
    </trdgrp>
    <trd lng="nl">naar buiten <ind>treden</ind></trd>
    <trd lng="ru">&c_v;&c_y;&c_j;&c_t;&c_i; <klr>(&c_i;&c_z; &c_r;&c_ja;&c_d;&c_a;)</klr></trd>
    <trdgrp lng="zh">
    <trd>&#x51FA;&#x8D70; <pr>ch&#x16B;z&#x1D2;u</pr></trd>,
    <trd>&#x51FA;&#22806; <pr>ch&#x16B;w&#xE0;i</pr></trd>,
    <trd>&#36920;&#x51FA; <pr>y&#xEC;ch&#x16B;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="pasx.el0i.iniciati">
    <dif>
      <ref cel="inicia.0i">Iniciati</ref> iun interrompantan diron a&ubreve; faron:
      <ekz>
        kiam la avino rakontis historiojn, li &ccirc;iam
        el<tld/>adis kun ia &leftquot;sed&rightquot;
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz><ekz>
        <klr>[tio]</klr> devigas ilin el<tld/>i kun sia projekto multe pli frue, ol ili
        intencis
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Dogmoj de Hilelismo</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ven.inter0i">interveni</ref>
    </refgrp>
    <trd lng="de">kommen <klr>(mit etwas)</klr></trd>
    <trd lng="hu">el&odblac;&aacute;ll (vmivel)</trd>
    <trdgrp lng="id">
      <trd><baz>sela</baz>menyela</trd>,
      <trd><baz>interupsi</baz>menginterupsi</trd>
    </trdgrp>
    <trd lng="nl">tussenbeide komen</trd>
    <trdgrp lng="zh">
    <trd>&#x51FA;&#28809; <pr>ch&#x16B;l&#xFA;</pr></trd>,
    <trd>&#x51FA;&#29200; <pr>ch&#x16B;l&#xFA;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="pasx.el0i.publike">
    <dif>
      Publike iniciati a&ubreve; <ref cel="deklar.0i">deklari</ref>:
      <ekz>
        efektive ekzistis homoj, kiuj el<tld/>adis kontra&ubreve; la pioniroj kun tiaj
        sensencaj kontra&ubreve;paroloj
        <fnt>
          <bib>EE</bib>
          <lok>I</lok>
        </fnt>;
      </ekz><ekz>
        kun propagando pli granda kaj publika ni el<tld/>os nur tiam, kiam post matura
        interkonsili&gcirc;o pri la detaloj <klr>[&#8230;]</klr> ni havos plenan certecon
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Nefermita letero al s-ro de Beaufront.</vrk>
        </fnt>;
      </ekz><ekz>
        inter vi jam ekzistas pacema rilato al tiaspecaj trockismaj el<tld/>oj
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lobi.0i">lobii</ref>,
      <ref cel="manife.0i">manifesti</ref>,
      <ref cel="prokla.0i">proklami</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>heraus <ind>kommen</ind> <klr>(mit etwas)</klr></trd>,
      <trd>antreten <klr>(gegen, mit etwas)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>fell&eacute;p</trd>,
      <trd>ki&aacute;ll</trd>,
      <trd>s&iacute;krasz&aacute;ll</trd>
    </trdgrp>
    <trd lng="nl">optreden</trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_y;&c_s;&c_t;&c_u;&c_p;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_s;&c_t;&c_u;&c_p;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <refgrp tip="vid">
    <ref cel="ir.el0i">eliri</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_j;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_v;&c_y;&c_s;&c_t;&c_u;&c_p;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>sortir du rang</trd>,
    <trd>intervenir<klr> (dans une discussion etc.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6B69;&#x307F;&#x51FA;&#x308B;  <pr>&#12354;&#12422;&#x307F;&#12391;&#x308B;</pr></trd>,
    <trd>&#x53E3;&#x51FA;&#12375;&#12377;&#x308B;  <pr>&#x304F;&#12385;&#12384;&#12375;&#12377;&#x308B;</pr></trd>,
    <trd>&#x4E57;&#x308A;&#x51FA;&#12377;  <pr>&#x306E;&#x308A;&#12384;&#12377;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wykracza&cacute;</trd>,
    <trd>wyst&eogonek;powa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>dep&abreve;&#537;e&#537;te</trd>,
    <trd>juca</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_s;&c_t;&c_u;&c_p;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_h;&c_o;&c_d;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
