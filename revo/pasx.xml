<drv mrk="pasx.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="pasx.0ado.irmaniero">
    <dif>
      Irado <tld/>on post <tld/>o:
      <ekz>
        &ccirc;e la krado eksonis malla&ubreve;ta <tld/>ado
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni a&ubreve;das <tld/>adon kaj tusetadon en la &ccirc;ambro
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen la sekvo de via dormanta <tld/>ado
        <fnt>
          <bib>ElektFab</bib>
          <lok>Kamarado Gaja</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_d;&c_a;</trd> 
  <trdgrp lng="br">
    <trd>bale<klr tip="ind"> (doare)</klr></trd>,
    <trd>kerzhed</trd>
  </trdgrp>
  <trd lng="de">Schreiten</trd>
  <trdgrp lng="fr">
    <trd>allure</trd>,
    <trd>d&eacute;marche</trd>
  </trdgrp>
  <trd lng="hu">j&aacute;r&aacute;s(m&oacute;d)</trd>
  <trdgrp lng="pl">
    <trd>bieg</trd>,
    <trd>ch&oacute;d</trd>
  </trdgrp> 
  <trdgrp lng="ro">
    <trd>curs</trd>,
    <trd>mers</trd>,
    <trd>plimbare</trd>
  </trdgrp>
  <trd lng="ru">&c_h;&c_o;&c_d;&c_mol;&c_b;&c_a;</trd>
  <trd lng="uk">&c_h;&c_o;&c_d;&c_mol;&c_b;&c_a;</trd>
</drv>

<drv mrk="pasx.al0i">
  <kap>al<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="pasx.al0i.KOMUNE">
    <dif>
      <tld lit="P"/>e <ref tip="super" cel="proksi.0igxi">proksimi&gcirc;i</ref> al iu a&ubreve; io:
      <ekz>
        la Izraelidoj al<tld/>is al la Benjamenidoj en la dua tago
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 20:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        apena&ubreve; &scirc;i al<tld/>is al la lito de la patro, li veki&gcirc;is
        <fnt>
          <bib>ElektFab</bib>
          <lok>La ora birdo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>hin gehen</trd>,
      <trd>heran nahen</trd>,
      <trd>n&auml;her kommen</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>odal&eacute;p</trd>,
      <trd>hozz&aacute;l&eacute;p</trd>
    </trdgrp>
    <trd lng="nl">aan komen <ind>lopen</ind></trd>
  </snc>
  <snc mrk="pasx.al0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ekfari, <ref tip="sin" cel="prov.ek0i">ekprovi</ref>, komenci <ref cel="trakt.0i.sinteni">trakti</ref>:
      <ekz>
        <klr>[ili]</klr> alpa&scirc;adis al la priju&gcirc;ado kun anta&ubreve;e jam preta konvinko
        <fnt>
          <bib>EE</bib>
          <lok>I</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili al<tld/>as al la elekto de lingvo internacia
        <fnt>
          <bib>EE</bib>
          <lok>V</lok>
        </fnt>;
      </ekz>
      <ekz>
        la a&ubreve;toroj de tiuj &ccirc;i projektoj al<tld/>as al ilia realigado
        <fnt>
          <bib>EE</bib>
          <lok>VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        lasu min al<tld/>i al la pruvoj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ir.al0i">aliri</ref>,
      <ref cel="proced.0i">procedi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>angehen</trd>,
      <trd>zur Tat <ind>schreiten</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>hozz&aacute;l&aacute;t</trd>,
      <trd>nekikezd</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>beginnen met</trd>,
      <trd>aanpakken</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_y;&c_s;&c_mol;&c_c;&c_ib;</trd>
  <trd lng="fr">approcher</trd>
  <trdgrp lng="pl">
    <trd>przyj&sacute;&cacute; pieszo</trd>,
    <trd>przyst&eogonek;powa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>vino pe jos</trd>,
    <trd>al&abreve;tura</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_o;&c_j;&c_t;&c_i;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_ib;&c_d;&c_h;&c_o;&c_d;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_n;&c_a;&c_b;&c_l;&c_i;&c_zh;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_s;&c_t;&c_u;&c_p;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="pasx.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="pasx.el0i.eliri">
    <dif>
      Eliri el la vico, anta&ubreve;eniri:
      <ekz>
        <nom>Edom</nom> el<tld/>is kontra&ubreve; lin kun popolo multenombra
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 20:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        el<tld/>u la pastro kaj parolu al la popolo
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 20:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        el amba&ubreve; veturiloj el<tld/>is po kvar viroj
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">heraus <ind>treten</ind></trd>
    <trd lng="hu">kil&eacute;p</trd>    
    <trd lng="nl">naar buiten <ind>treden</ind></trd>
    <trd lng="ru">&c_v;&c_y;&c_j;&c_t;&c_i; <klr>(&c_i;&c_z; &c_r;&c_ja;&c_d;&c_a;)</klr></trd>
  </snc>
  <snc mrk="pasx.el0i.iniciati">
    <dif>
      <ref cel="inicia.0i">Iniciati</ref> iun interrompantan diron a&ubreve; faron:
      <ekz>
        kiam la avino rakontis historiojn, li &ccirc;iam
        el<tld/>adis kun ia &leftquot;sed&rightquot;
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tio]</klr> devigas ilin el<tld/>i kun sia projekto multe pli frue, ol ili
        intencis
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Dogmoj de Hilelismo</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ven.inter0i">interveni</ref>
    </refgrp>
    <trd lng="de">kommen <klr>(mit etwas)</klr></trd>
    <trd lng="hu">el&odblac;&aacute;ll (vmivel)</trd>    
    <trd lng="nl">tussenbeide komen</trd>
  </snc>
  <snc mrk="pasx.el0i.publike">
    <dif>
      Publike iniciati a&ubreve; <ref cel="deklar.0i">deklari</ref>:
      <ekz>
        efektive ekzistis homoj, kiuj el<tld/>adis kontra&ubreve; la pioniroj kun tiaj
        sensencaj kontra&ubreve;paroloj
        <fnt>
          <bib>EE</bib>
          <lok>I</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun propagando pli granda kaj publika ni el<tld/>os nur tiam, kiam post matura
        interkonsili&gcirc;o pri la detaloj <klr>[&#8230;]</klr> ni havos plenan certecon
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Nefermita letero al s-ro de Beaufront.</vrk>
        </fnt>;
      </ekz>
      <ekz>
        inter vi jam ekzistas pacema rilato al tiaspecaj trockismaj el<tld/>oj
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lobi.0i">lobii</ref>,
      <ref cel="manife.0i">manifesti</ref>,
      <ref cel="prokla.0i">proklami</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>heraus <ind>kommen</ind> <klr>(mit etwas)</klr></trd>,
      <trd>antreten <klr>(gegen, mit etwas)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>fell&eacute;p</trd>,
      <trd>ki&aacute;ll</trd>,
      <trd>s&iacute;krasz&aacute;ll</trd>
    </trdgrp>
    <trd lng="nl">optreden</trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_y;&c_s;&c_t;&c_u;&c_p;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_s;&c_t;&c_u;&c_p;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <refgrp tip="vid">
    <ref cel="ir.el0i">eliri</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_j;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_v;&c_y;&c_s;&c_t;&c_u;&c_p;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="fr">prendre une <ind>initiative</ind></trd>
  <trdgrp lng="pl">
    <trd>wykracza&cacute;</trd>,
    <trd>wyst&eogonek;powa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>dep&abreve;&scedil;e&scedil;te</trd>,
    <trd>juca</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_s;&c_t;&c_u;&c_p;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_h;&c_o;&c_d;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="pasx.mis0o">
  <kap>mis<tld/>o</kap>
  <snc mrk="pasx.mis0o.stumblo">
    <dif>
      Mallerta <tld/>o,
      <ref tip="vid" cel="stumbl.0o.KOMUNE">stumblo</ref>:
      <ekz>
        dum montgrimpado <nom>Li Zhixin</nom> ofte estis trafita de ventego, 
        lavango, falado pro mis<tld/>o
        <fnt>
          <aut>Zuo Yan</aut>,
          <vrk><url ref="http://investchina.org.cn/world/shi-living/sporto/56.htm">
          Alpismo &ndash; mia kariero</url></vrk>,
          <lok>[2006]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="id">ter<ind>peleset</ind></trd>
  </snc>
  <snc mrk="pasx.mis0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Maltakta, mal&gcirc;usta, mallerta ago:
      <ekz>
        &ccirc;iuj eraras kaj foje stulte agas. La &ccirc;efa afero estas konscii 
        pri tio kaj korekti la mis<tld/>ojn
        <fnt>
          <aut>V. Markov</aut>,
          <vrk><url ref="http://www.satesperanto.org/Kio-estas-respekto.html">
          Kio estas respekto?</url></vrk>,
          <lok>La Juna Penso, 2001-07</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="id"><ind>salah</ind> langkah</trd>
  </snc>
  <trd lng="de">Fehltritt</trd>
  <trd lng="en">misstep</trd>
  <trd lng="fr">faux-pas</trd>
  <trd lng="nl">misstap</trd>
  <trdgrp lng="pl">
    <trd>dewiacja</trd>,
    <trd>odchylenie</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>deviatie</trd>,
    <trd>divergen&#539;&abreve;</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_je;&c_v;&c_je;&c_r;&c_n;&c_y;&c_j;
  &c_sh;&c_a;&c_g;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_m;&c_i;&c_l;&c_k;&c_o;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_r;&c_a;&c_v;&c_i;&c_l;&c_mol;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_h;&c_i;&c_b;&c_n;&c_i;&c_j; &c_k;&c_r;&c_o;&c_k;/&c_v;&c_ch;&c_i;&c_n;&c_o;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="pasx.super0itrans0i">
  <kap>super<tld/>i, <var><kap>trans<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Iri trans, pli malproksime ol:
      <ekz>
        vi en nenio super<tld/>u la moderecon de la naturo
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li iom super<tld/>is ne nur sian, sed anka&ubreve; nian epokon
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470;5</lok>
        </fnt>;
      </ekz>
      <ekz>
        riveron oni trans<tld/>is piede
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 66:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        netrans<tld/>ebla baro;
      </ekz>
      <ekz>
        Li donis sian le&gcirc;on al la maro, por ke la akvoj
        ne trans<tld/>u siajn bordojn
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 8:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        Vi faris limon, kiun &gcirc;i ne super<tld/>os
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 104:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        kial viaj dis&ccirc;iploj trans<tld/>as la tradicion de la anta&ubreve;uloj
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 15:2</lok>
        </fnt>?
      </ekz>
      <ekz>
        e&ccirc; unu vorteto ne trans<tld/>os niajn lipojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tisto de porkoj</lok>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_je;&c_r;&c_a;&c_j;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_s;&c_t;&c_u;&c_p;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">&uuml;berschreiten</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;passer</trd>,
    <trd>enjamber</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>t&uacute;ll&eacute;p</trd>,
    <trd>meghalad</trd>
  </trdgrp>
  <trd lng="nl">te boven <ind>gaan</ind></trd>
  <trdgrp lng="pl">
    <trd>przekracza&cacute;</trd>,
    <trd>przest&aogonek;pi&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>dep&abreve;&#537;i</trd>,
    <trd>pas peste</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_j;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_s;&c_t;&c_u;&c_p;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="pasx.sxtel0a">
  <kap>&scirc;tel<tld/>a</kap>
  <snc mrk="pasx.sxtel0a.KOMUNE">
    <dif>
      Senbrue <tld/>anta, movi&gcirc;anta, aganta:
      <ekz>
        neniam a&ubreve;di&gcirc;is e&ccirc; unu sola lamento de &ccirc;i virino &scirc;tel-<tld/>a 
        kaj nepenetrebla
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;tel<tld/>a valeto kondukis min de tie, en silento, tra multaj 
        malhelaj kaj kompleksaj koridoroj
        <fnt>
          <aut>E. A. Poe, trad. E. Grobe</aut>,
          <vrk><url ref="&Gutenberg;etext/17425">
          La Falo de U&scirc;ero-Domo</url></vrk>,
          <lok>Arizona-Stelo-Eldonejo, 2000</lok>
        </fnt>.
      </ekz>
    </dif>
<ref tip="vid" cel="ir.sxtel0i">&scirc;teliri</ref>
  </snc>
  <trd lng="de">schleichend</trd>
  <trdgrp lng="fr">
    <trd>allant <ind>&agrave; pas de loup</ind></trd>,
    <trd>discret<klr> (se d&eacute;pla&ccedil;ant sans bruit)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ukradkowy</trd>,
    <trd>dyskretny  <klr>(porusza si&eogonek; bezg&lstroke;o&sacute;nie)</klr></trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>ascunde</trd>,
    <trd>discret  <klr>(se mi&#537;c&abreve; &icirc;n t&abreve;cere)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="pasx.0npost0">
  <kap><tld/>on post <tld/>o,
  <var><kap><tld/>o post <tld/>o</kap></var>,
  <var><kap><tld/>-<tld/>e</kap></var></kap>
  <snc mrk="pasx.0npost0.KOMUNE">
    <uzo tip="stl">FRAZ</uzo>
    <dif>
      <ref tip="vid" cel="iom.0post0.KOMUNE">Iom post iom</ref>,
      per grada, la&ubreve;tempa evoluo:
      <ekz>
        ni <tld/>o post <tld/>o, post longa laboro, atingos la celon
        <fnt>
          <aut>L. L. Zamenhof</aut>
          <vrk>La Vojo</vrk>
          <lok>en: Fundamenta Krestomatio, 1904</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili devis iri anta&ubreve;en <tld/>on post <tld/>o
        <fnt>
          <bib>Far2</bib>
          <lok>&ccirc;apitro 19a</lok>
        </fnt>;
      </ekz>
      <ekz>
        nur <tld/>o post <tld/>o <klr>[li]</klr> progresigas la leganton al komprenado
        de pli komplikaj frazoj
        <fnt>
          <bib>Plumamikoj</bib>
          <lok>Kie komencu komencanto?</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni ekkonu Lin <tld/>-<tld/>e <klr>[&#8230;]</klr> ni proksimi&gcirc;u al Li per la pre&gcirc;ado
        <fnt>
          <aut>J. Ratzinger, trad. B. Eichkorn</aut>
          <vrk><url ref="http://bernhardeichkorn.de/ratzinger-patronia/">La Patronia</url></vrk>
          <lok>2019</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd><ind>Schritt</ind> f&uuml;r Schritt</trd>,
    <trd>in kleinen <ind>Schritt</ind>en</trd>
  </trdgrp>
  <trd lng="fr">pas &agrave; pas</trd>
  <trdgrp lng="pl">
    <trd>krok za krokiem</trd>,
    <trd>krok po kroku</trd>
  </trdgrp>
  <trd lng="ro">pu&tcedil;in c&acirc;te pu&tcedil;in</trd>
</drv>

<drv mrk="pasx.0stango">
  <kap><tld/>stango</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="ir.0ilo">Irilo</ref>:
      <ekz>
        maskoj, kostumoj, danco, <tld/>stangoj kaj spektakloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=520299"
              >Karnavalo en Bremeno</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="stilz.0o">stilzo</ref>
  </snc>
  <trd lng="be">&c_m;&c_y;&c_l;&c_ib;&c_c;&c_a;</trd>
  <trd lng="de">Stelze</trd>
  <trd lng="fr">&eacute;chasse<klr> (pour marcher)</klr></trd>
  <trd lng="hu">g&oacute;lyal&aacute;b</trd>  
  <trdgrp lng="id">
    <trd>egrang</trd>,
    <trd><ind>jangkung</ind>an</trd>
  </trdgrp>
  <trd lng="nl">wandelstok</trd>
  <trd lng="pl">szczud&lstroke;o</trd>
  <trd lng="ro">piciorong</trd>
  <trd lng="ru">&c_h;&c_o;&c_d;&c_u;&c_l;&c_ja;</trd>
</drv>
