<drv mrk="pasx.sxtel0a">
  <kap>&scirc;tel<tld/>a</kap>
  <snc mrk="pasx.sxtel0a.KOMUNE">
    <dif>
      Senbrue <tld/>anta, movi&gcirc;anta, aganta:
      <ekz>
        neniam a&ubreve;di&gcirc;is e&ccirc; unu sola lamento de &ccirc;i virino &scirc;tel-<tld/>a
        kaj nepenetrebla
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent
