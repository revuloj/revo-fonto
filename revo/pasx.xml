<drv mrk="pasx.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="pasx.0ado.irmaniero">
    <dif>
      Irado <tld/>on post <tld/>o:
      <ekz>
        &ccirc;e la krado eksonis malla&ubreve;ta <tld/>ado
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro IV</lok>
        </fnt>;
      </ekz><ekz>
        oni a&ubreve;das <tld/>adon kaj tusetadon en la &ccirc;ambro
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz><ekz>
        jen la sekvo de via dormanta <tld/>ado
        <fnt>
          <bib>ElektFab</bib>
          <lok>Kamarado Gaja</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_d;&c_a;</trd>
  <trdgrp lng="br">
    <trd>bale<klr tip="ind"> (doare)</klr></trd>,
    <trd>kerzhed</trd>
  </trdgrp>
  <trd lng="de">Schreiten</trd>
  <trdgrp lng="fr">
    <trd>allure</trd>,
    <trd>d&eacute;marche</trd>
  </trdgrp>
  <trd lng="hu">j&aacute;r&aacute;s(m&oacute;d)</trd>
  <trd lng="ja">&#x6B69;&#x884C;  <pr>&#x304B;&#12385;</pr></trd>
  <trdgrp lng="pl">
    <trd>bieg</trd>,
    <trd>ch&oacute;d</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>curs</trd>,
    <trd>mers</trd>,
    <trd>plimbare</trd>
  </trdgrp>
  <trd lng="ru">&c_h;&c_o;&c_d;&c_mol;&c_b;&c_a;</trd>
  <trd lng="uk">&c_h;&c_o;&c_d;&c_mol;&c_b;&c_a;</trd>
</drv>

<drv mrk="pasx.al0i">
  <kap>al<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="pasx.al0i.KOMUNE">
    <dif>
      <tld lit="P"/>e <ref tip="super" cel="proksi.0igxi">proksimi&gcirc;i</ref> al iu a&ubreve; io:
      <ekz>
        la Izraelidoj al<tld/>is al la Benjamenidoj en la dua tago
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 20:24</lok>
        </fnt>;
      </ekz><ekz>
        apena&ubreve; &scirc;i al<tld/>is al la lito de la patro, li veki&gcirc;is
        <fnt>
          <bib>ElektFab</bib>
          <lok>La ora birdo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>hin gehen</trd>,
      <trd>heran nahen</trd>,
      <trd>n&auml;her kommen</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>odal&eacute;p</trd>,
      <trd>hozz&aacute;l&eacute;p</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd><baz>langkah</baz>melangkah menuju</trd>,
      <trd><baz>dekat</baz>mendekati</trd>
    </trdgrp>
    <trd lng="nl">aan komen <ind>lopen</ind></trd>
    <trdgrp lng="zh">
