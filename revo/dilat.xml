<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dilat.xml,v 1.19 2021/03/08 15:14:34 revo Exp $">
<kap>
  <rad>dilat</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="dilat.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="dilat.0i.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Vastigi, grandigi la amplekson de korpo sen aldono
      de materio, maldensigante &gcirc;ian substancon:
      <ekz>
        varmo <tld/>as la korpojn;
      </ekz>
    </dif>
  </snc>
  <snc mrk="dilat.0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Vastigi, lar&gcirc;igi organon a&ubreve; malferma&jcirc;on:
      <ekz>
        <ind><tld/>ado</ind> de la uretra
        kanalo<fnt><bib>PIV1</bib></fnt>;
        <trd lng="fr">dilatation<klr tip="ind"> (action)</klr></trd>
        <trd lng="hu">(ki)<ind>t&aacute;g&iacute;t&aacute;s</ind></trd>
      </ekz>
      <ekz>
        <ind>angi<tld/>a</ind> drogo <klr>(pligrandiganta la
        diametron de angioj)</klr>.
        <trd lng="fr">vasodilatateur</trd>
        <trd lng="hu">&eacute;rt&aacute;g&iacute;t&oacute;</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_y;&c_r;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_sh;&c_y;&c_r;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_g;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dilatovat</trd>,
    <trd>rozp&iacute;nat <klr>(objem)</klr></trd>,
    <trd>rozt&aacute;hnout</trd>,
    <trd>roz&scaron;&iacute;&rcaron;it</trd>
  </trdgrp>
  <trd lng="de">dehnen</trd>
  <trd lng="en">dilate</trd>
  <trd lng="es">dilatar</trd>
  <trd lng="fr">dilater</trd>
  <trd lng="he">&#x5DC;&#x5D4;&#x5E8;&#x5D7;&#x5D9;&#x5D1;</trd>
  <trd lng="hu">(ki)<ind>t&aacute;g&iacute;t</ind></trd>
  <trdgrp lng="pl">
    <trd>rozszerza&cacute;</trd>,
    <trd>rozwiera&cacute;</trd>
  </trdgrp><trd lng="pl">rozszerza&cacute;</trd>
  <trd lng="pt">dilatar</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_i;&c_r;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_t;&c_ja;&c_g;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rozopn&uacute;&tcaron;</trd>,
    <trd>roztiahnu&tcaron;</trd>,
    <trd>roz&scaron;&iacute;ri&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="dilat.0o">
  <kap><tld/>o</kap>
  <snc mrk="dilat.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Fenomeno, en kiu la amplekso a&ubreve; la volumeno de gasa, likva
      a&ubreve; solida substanco pligrandi&gcirc;as.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_y;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_a;&c_sh;&c_y;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_g;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>rozta&zcaron;ivost</trd>,
    <trd>rozta&zcaron;nost</trd>
  </trdgrp>
  <trd lng="de">Dehnung</trd>
  <trd lng="en">dilation</trd>
  <trd lng="es">dilataci&oacute;n</trd>
  <trd lng="fr">dilatation<klr tip="ind">
  (ph&eacute;nom&egrave;ne)</klr></trd>
  <trd lng="he">&#x5DE;&#x5D5;&#x5E8;&#x5D7;&#x5D1;</trd>
  <trdgrp lng="hu">
    <trd>t&aacute;g&iacute;t&aacute;s</trd>,
    <trd>dilat&aacute;l&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dylatacja</trd>,
    <trd>rozszerzalno&sacute;&cacute;</trd>
  </trdgrp>
  <trd lng="pt">dilata&ccedil;&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_i;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_t;&c_ja;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rozp&iacute;nanie</trd>,
    <trd>roztiahnutie</trd>,
    <trd>roz&scaron;&iacute;renie</trd>
  </trdgrp>
</drv>

<drv mrk="dilat.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="dilat.0igxi.FIZ">
    <dif>
      Plivasti&gcirc;i, plilar&gcirc;i&gcirc;i:
      <ekz>
        la okuloj de mia patrino <tld/>i&gcirc;is pro hororo
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_y;&c_r;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_sh;&c_y;&c_r;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_g;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de">sich <ind>ausdehnen</ind></trd>
  <trd lng="en">dilate</trd>
  <trd lng="fr">se <ind>dilater</ind></trd>
  <trd lng="he">&#x5DC;&#x5B0;&#x5D4;&#x5B4;&#x5EA;&#x5B0;&#x5E8;&#x5B7;&#x5D7;&#x5B5;&#x5D1;</trd>
  <trd lng="hu">(ki)<ind>t&aacute;gul</ind></trd>  
  <trd lng="pl">rozszerza&cacute; si&eogonek;</trd>
  <trd lng="pt">dilatar-se</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_i;&c_r;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_t;&c_ja;&c_g;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="dilat.0anto">
  <kap><tld/>anto</kap>
  <snc mrk="dilat.0anto.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Muskolo, kiu <tld/>as organon.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_g;&c_ib;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>dilat&aacute;tor</trd>,
    <trd>sval roz&scaron;i&rcaron;ova&ccaron;</trd>
  </trdgrp>
  <trd lng="de">Strecker</trd>
  <trd lng="fr">dilatateur<klr tip="ind"> (muscle)</klr></trd>
  <trd lng="he">&#x5DE;&#x5B7;&#x5E8;&#x5D7;&#x5B4;&#x5D9;&#x5D1;</trd>
  <trd lng="hu">t&aacute;g&iacute;t&oacute;izom</trd>
  <trd lng="pl">rozwieracz</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_i;&c_l;&c_a;&c_t;&c_a;&c_t;&c_o;&c_r;</trd>,
    <trd>&c_m;&c_y;&c_sh;&c_c;&c_a;-&c_r;&c_a;&c_s;&c_sh;&c_i;&c_r;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">sval roz&tcaron;ahova&ccaron;</trd>
</drv>

<drv mrk="dilat.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="dilat.0ilo.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Ilo, uzata por <tld/>i orificon.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_sh;&c_y;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trd lng="de">Dilator</trd>
  <trd lng="en">dilator</trd>
  <trd lng="fr">dilatateur<klr tip="ind"> (instrument)</klr></trd>
  <trd lng="he">&#x5DE;&#x5B7;&#x5E8;&#x5D7;&#x5B4;&#x5D9;&#x5D1;</trd>
  <trdgrp lng="hu">
    <trd>t&aacute;g&iacute;t&oacute;eszk&ouml;z</trd>,
    <trd>dilator</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_sh;&c_i;&c_r;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
</drv>
</art>
