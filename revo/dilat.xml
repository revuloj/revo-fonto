<vortaro>
<art mrk="$Id: dilat.xml,v 1.27 2025/06/03 06:00:22 revo Exp $">
<kap>
  <rad>dilat</rad>/i <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="dilat.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="dilat.0i.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      <ref tip="super" cel="vast.0igi.grandigi">Vastigi</ref>, grandigi la amplekson 
      de korpo sen aldono de materio, maldensigante &gcirc;ian substancon:
      <ekz>
        varmo <tld/>as la korpojn;
      </ekz>
      <ekz>
        &ccirc;e fervojaj reloj oni anta&ubreve;vidas etajn bre&ccirc;ojn <klr>[&ccirc;ar alie]</klr>, ili
        kurbi&gcirc;us pro la pu&scirc;o de <tld/>ado
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011332.php">Dilati&gcirc;o, kuntiri&gcirc;o ... kaj eksperimento por
          infana amuzi&gcirc;o</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="etend.0i">etendi</ref>,
      <ref cel="sxvel.0igi">&scirc;veligi</ref>
    </refgrp>
    <trd lng="de">ausdehnen</trd>
    <trdgrp lng="zh">
    <trd>&#23485;&#38480; <pr>ku&#257;nxi&agrave;n</pr></trd>,
    <trd>&#20280; <pr>sh&#275;n</pr></trd>,
    <trd>&#30003; <pr>sh&#275;n</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="dilat.0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Vastigi, lar&gcirc;igi organon a&ubreve; malferma&jcirc;on:
      <ekz>
        <ind><tld/>ado</ind> de la uretra
        kanalo
        <fnt><bib>PIV1</bib></fnt>;
        <trd lng="fr">dilatation<klr tip="ind"> (action)</klr></trd>
        <trd lng="hu">(ki)<ind>t&aacute;g&iacute;t&aacute;s</ind></trd>
      </ekz>
      <ekz>
        fumado <klr>[&#8230;]</klr> havas efikon mal<tld/>an
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2013/011124.php">Atentu la koron</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>angi<tld/>a</ind> drogo <klr>(pligrandiganta la
        diametron de angioj)</klr>.
        <trd lng="de">gef&auml;&szlig;erweiternd</trd>
        <trd lng="fr">vasodilatateur</trd>
        <trd lng="hu">&eacute;rt&aacute;g&iacute;t&oacute;</trd>
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>dehnen</trd>,
      <trd>erweitern</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_y;&c_r;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_sh;&c_y;&c_r;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_g;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">dilatar</trd>
  <trdgrp lng="cs">
    <trd>dilatovat</trd>,
    <trd>rozp&iacute;nat <klr>(objem)</klr></trd>,
    <trd>rozt&aacute;hnout</trd>,
    <trd>roz&scaron;&iacute;&rcaron;it</trd>
  </trdgrp>
  
  <trd lng="en">dilate</trd>
  <trd lng="es">dilatar</trd>
  <trd lng="fr">dilater</trd>
  <trd lng="he">&lamed;&he;&resh;&het;&yod;&bet;</trd>
  <trd lng="hu">(ki)<ind>t&aacute;g&iacute;t</ind></trd>
  <trd lng="ja">&#33192;&#24373;&#12373;&#12379;&#12427;  <pr>&#12412;&#12358;&#12385;&#12423;&#12358;&#12373;&#12379;&#12427;</pr></trd>
  <trdgrp lng="pl">
    <trd>rozszerza&cacute;</trd>,
    <trd>rozwiera&cacute;</trd>
  </trdgrp><trd lng="pl">rozszerza&cacute;</trd>
  <trd lng="pt">dilatar</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_i;&c_r;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_t;&c_ja;&c_g;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rozopn&uacute;&tcaron;</trd>,
    <trd>roztiahnu&tcaron;</trd>,
    <trd>roz&scaron;&iacute;ri&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_z;&c_sh;&c_i;&c_r;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_t;&c_ja;&c_g;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="dilat.0o">
  <kap><tld/>o</kap>
  <snc mrk="dilat.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      <ref tip="super" cel="fenome.0o">Fenomeno</ref>, &ccirc;e kiu la amplekso a&ubreve; 
      la volumeno de gasa, likva a&ubreve; solida substanco pligrandi&gcirc;as:
      <ekz>
        <klr>[jen]</klr> efikoj de la <tld/>o de la ligno
        <fnt>
          <aut>Kardec Allan</aut>
          <vrk><url ref="https://royallib.com/read/Kardec_Allan/La_libro_de_la_spiritoj.html">La
          libro de la spiritoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_y;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_a;&c_sh;&c_y;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_g;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="ca">dilataci&oacute;</trd>
  <trdgrp lng="cs">
    <trd>rozta&zcaron;ivost</trd>,
    <trd>rozta&zcaron;nost</trd>
  </trdgrp>
  <trd lng="de">Dehnung</trd>
  <trd lng="en">dilation</trd>
  <trd lng="es">dilataci&oacute;n</trd>
  <trd lng="fr">dilatation<klr tip="ind">
  (ph&eacute;nom&egrave;ne)</klr></trd>
  <trd lng="he">&mem;&vav;&resh;&het;&bet;</trd>
  <trdgrp lng="hu">
    <trd>t&aacute;g&iacute;t&aacute;s</trd>,
    <trd>dilat&aacute;l&aacute;s</trd>
  </trdgrp>
  <trd lng="ja">&#33192;&#24373;  <pr>&#12412;&#12358;&#12385;&#12423;&#12358;</pr></trd>
  <trdgrp lng="pl">
    <trd>dylatacja</trd>,
    <trd>rozszerzalno&sacute;&cacute;</trd>
  </trdgrp>
  <trd lng="pt">dilata&ccedil;&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_i;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_t;&c_ja;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rozp&iacute;nanie</trd>,
    <trd>roztiahnutie</trd>,
    <trd>roz&scaron;&iacute;renie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_i;&c_l;&c_a;&c_t;&c_a;&c_c;&c_ib;&c_ja;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_sh;&c_i;&c_r;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_t;&c_ja;&c_g;&c_n;&c_je;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#24310;&#20280;&#29575; <pr>y&aacute;nsh&#275;nshu&agrave;i</pr></trd>,
    <trd>&#25289;&#30699;&#29575; <pr>l&#257;ji&#462;oshu&agrave;i</pr></trd>,
    <trd>&#20280;&#38263; <pr>sh&#275;nch&aacute;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="dilat.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="dilat.0igxi.FIZ">
    <dif>
      <ref tip="super" cel="vast.pli0igxi">Plivasti&gcirc;i</ref>, plilar&gcirc;i&gcirc;i:
      <ekz>
        la celo de tiaj fendoj <klr>[estas]</klr> ebligi al betono <tld/>i&gcirc;i dum plej
        varmaj tagoj, sen provoki la rompi&gcirc;on de la pavimo
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011332.php">Dilati&gcirc;o, kuntiri&gcirc;o 
          ... kaj eksperimento por infana amuzi&gcirc;o</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni pensis, ke la marnivelo plialti&gcirc;as precipe pro la term<klr>[ik]</klr>a
        <tld/>i&gcirc;o de la supraj, rapide ruli&gcirc;antaj kelkcent metroj, kiuj estas
        varmigataj de la suno
        <fnt>
          <bib>Monato</bib>
          <aut>Werner Fu&szlig;</aut>
          <vrk><url ref="&Monato;/2012/010695.php">Klimat&scirc;an&gcirc;aj fabeloj? Tute ne.</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la okuloj de mia patrino <tld/>i&gcirc;is 
        <klr>(<ref cel="grand.0igxi">grandi&gcirc;is</ref>)</klr> pro hororo
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_y;&c_r;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_sh;&c_y;&c_r;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_g;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">dilatar-se</trd>
  <trd lng="de">sich <ind>ausdehnen</ind></trd>
  <trd lng="en">dilate</trd>
  <trd lng="es">dilatarse</trd>
  <trd lng="fr">se <ind>dilater</ind></trd>
  <trd lng="he">&lamed;&#1456;&he;&#1460;&tav;&#1456;&resh;&#1463;&het;&#1461;&bet;</trd>
  <trd lng="hu">(ki)<ind>t&aacute;gul</ind></trd>  
  <trd lng="ja">&#33192;&#24373;&#12377;&#12427;  <pr>&#12412;&#12358;&#12385;&#12423;&#12358;&#12377;&#12427;</pr></trd>
  <trd lng="pl">rozszerza&cacute; si&eogonek;</trd>
  <trd lng="pt">dilatar-se</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_i;&c_r;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_t;&c_ja;&c_g;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_z;&c_sh;&c_i;&c_r;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_t;&c_ja;&c_g;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="zh">&#32960; <pr>zh&agrave;ng</pr></trd>
</drv>

<drv mrk="dilat.0anto">
  <kap><tld/>anto</kap>
  <snc mrk="dilat.0anto.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      <ref tip="super" cel="muskol.0o">Muskolo</ref>, kiu <tld/>as organon.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_g;&c_ib;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trd lng="ca">m&uacute;scul dilatador</trd>
  <trdgrp lng="cs">
    <trd>dilat&aacute;tor</trd>,
    <trd>sval roz&scaron;i&rcaron;ova&ccaron;</trd>
  </trdgrp>
  <trd lng="de">Strecker</trd>
  <trd lng="fr">dilatateur<klr tip="ind"> (muscle)</klr></trd>
  <trd lng="he">&mem;&#1463;&resh;&het;&#1460;&yod;&bet;</trd>
  <trd lng="hu">t&aacute;g&iacute;t&oacute;izom</trd>
  <trd lng="pl">rozwieracz</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_i;&c_l;&c_a;&c_t;&c_a;&c_t;&c_o;&c_r;</trd>,
    <trd>&c_m;&c_y;&c_sh;&c_c;&c_a;-&c_r;&c_a;&c_s;&c_sh;&c_i;&c_r;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">sval roz&tcaron;ahova&ccaron;</trd>
</drv>

<drv mrk="dilat.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="dilat.0ilo.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Ilo, uzata por <tld/>i orificon.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_sh;&c_y;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="ca">
    <trd>dilatador</trd>,
    <trd>esp&egrave;cul</trd>
  </trdgrp>
  <trd lng="de">Dilator</trd>
  <trd lng="en">dilator</trd>
  <trd lng="fr">dilatateur<klr tip="ind"> (instrument)</klr></trd>
  <trd lng="he">&mem;&#1463;&resh;&het;&#1460;&yod;&bet;</trd>
  <trdgrp lng="hu">
    <trd>t&aacute;g&iacute;t&oacute;eszk&ouml;z</trd>,
    <trd>dilator</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_sh;&c_i;&c_r;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_i;&c_l;&c_a;&c_t;&c_a;&c_t;&c_o;&c_r;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_sh;&c_i;&c_r;&c_ju;&c_v;&c_a;&c_ch;</trd>
  </trdgrp>
</drv>
</art>
