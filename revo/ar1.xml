<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>ar</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="ar1.0o">
  <kap><tld/>o</kap>
  <mlg>a</mlg>
  <snc>
    <dif>
      Komunuza <ref tip="lst" cel="unu.mezur0o"
      lst="voko:areoj">mezurunuo</ref> de
      areo, uzata &ccirc;efe por mezuri terenon, egala al cent 
      <ref tip="prt" cel="metr.kvadrata0o">kvadrataj metroj</ref>:
      <ekz>
        akreo imperia &dash; 40 aroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref='https://eo.wikipedia.org/?curid=2789'>Mezurunuo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="hom" cel="ar.0o">aro <klr>(kolekto)</klr></ref>
    <adm>La zamenhofeco de tiu vorto ne estas pruvita per fonto</adm>
  </snc>
  <trd lng="be">&c_a;&c_r;</trd>
  <trd lng="bg">&c_a;&c_r;</trd>
  <trd lng="br">ar<klr tip="ind"> (muzul)</klr></trd>
  <trdgrp lng="cs">
    <trd>houf</trd>,
    <trd>nastaven&iacute;</trd>,
    <trd>sada</trd>,
    <trd>skupina</trd>,
    <trd>soubor</trd>,
    <trd>souhrn</trd>,
    <trd>soustava</trd>,
    <trd>s&eacute;rie</trd>,
    <trd>&rcaron;ada</trd>
  </trdgrp>
  <trd lng="de">Ar</trd>
  <trd lng="en">are</trd>
  <trd lng="es">&aacute;rea</trd>
  <trd lng="fa">&a_A_madda;&a_r;</trd>
  <trd lng="fr">are</trd>
  <trd lng="hu">&aacute;r<klr tip="ind"> (ter&uuml;letm&eacute;rt&eacute;k)</klr></trd>
  <trd lng="id">are</trd>
  <trd lng="it">ara<klr tip="ind"> (unit&agrave; di misura)</klr></trd>
  <trd lng="ru">&c_a;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>&aacute;r</trd>,
    <trd>sada</trd>,
    <trd>s&uacute;bor</trd>,
    <trd>s&uacute;stava</trd>
  </trdgrp>
</drv>
</art>
