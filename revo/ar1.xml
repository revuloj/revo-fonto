  <fnt>Z</fnt>
</kap>
<drv mrk="ar1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Komunuza <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de areo, 
      uzata &ccirc;efe por mezuri terenon, egala al cent 
      <ref tip="prt" cel="metr.kvadrata0o">kvadrataj metroj</ref>; simb. a.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;</trd>        
  <trd lng="br">ar<klr tip="ind"> (muzul)</klr></trd>
  <trd lng="cs">souhrn</trd>
  <trd lng="en">are</trd>
  <trd lng="es">&aacute;rea</trd>
  <trd lng="fr">are</trd>
  <trd lng="hu">&aacute;r<klr tip="ind"> (ter&uuml;letm&eacute;rt&eacute;k)</klr></trd>
  <trd lng="ru">&c_a;&c_r;</trd>
  <trd lng="sk">&aacute;r</trd>
</drv>
</art>
