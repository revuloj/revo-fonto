<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: akreci.xml,v 1.5 2022/04/25 15:10:59 revo Exp $">
<kap>
  <rad>akreci</rad>/o
</kap>
<drv mrk="akreci.0o">
  <kap><tld/>o</kap>
  <snc mrk="akreci.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="vid" cel="amas.0igxo.KOMUNE">Amasi&gcirc;o</ref>,
      densi&gcirc;o de materio &ccirc;e anta&ubreve;ekzista kerno:
      <ekz>
        aperas <tld/>a disko, la substanco de kiu poiomete 
        faladas sur la surfacon de &ccirc;efa stelo
        <fnt>
          <aut>I. L. Andronov</aut>,
          <vrk><url ref="&STEB;astronomio/stelo/stelo.htm">
          La Strukturo de Magnetaj Malvastaj Duoblaj Stelaj
          Sistemoj</url></vrk>,
          <lok>1989</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a nivelo estas maksimuma, kiam la angulo <klr>[&#8230;]</klr> inter magneta akso
        kaj linio da centroj estas nula
        <fnt>
          <vrk><url ref="&STEB;astronomio/stelo/stelo.htm">La Strukturo De
          Magnetaj Malvastaj Duoblaj Stelaj Sistemoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aglome.0ajxo">aglomera&jcirc;o</ref>,
      <ref cel="ekskre.0o.FIZL">ekskrecio</ref>,
      <ref cel="sekrec.0o.FIZL">sekrecio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_r;&c_e;&c_c;&c_y;&c_ja;</trd>
  <trd lng="de">Akkretion</trd>
  <trd lng="en">accretion</trd>
  <trd lng="fr">accr&eacute;tion</trd>
</drv>
</art>

