<art mrk="$Id: vitre.xml,v 1.7 2022/11/30 18:35:13 revo Exp $">
<kap>
  <rad>vitre</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="vitre.0o">
  <kap><tld/>o</kap>
  <snc mrk="vitre.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Gelateneca substanco, kiu plenigas la internan
      parton de la okulo malanta&ubreve; la lenso:
      <ekz>
        malfiksi&gcirc;o de la posta <tld/>o estas ordinara afero 
        kaj ne estas dan&gcirc;era
        <fnt>
         <aut>[K. Lendon]</aut>,
         <vrk><url ref="&STEB;medicino/flosajxoj/flosajxoj.htm">
         Flosa&jcirc;oj</url></vrk>,
         <lok>1999-2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_k;&c_l;&c_ib;&c_s;&c_t;&c_a;&c_je; &c_c;&c_je;&c_l;&c_a;</trd>
  <trd lng="bg">&c_k;&c_o;&c_r;&c_p;&c_u;&c_s; &c_v;&c_i;&c_t;&c_r;&c_je;&c_u;&c_m;</trd>
  <trd lng="cs">sklivec</trd>
  <trd lng="de">Glask&ouml;rper</trd>
  <trdgrp lng="en">
    <trd>vitreous body</trd>,
    <trd>vitreous humour</trd>
  </trdgrp>
  <trd lng="fr">corps vitr&eacute;</trd>
  <trd lng="la">corpus vitreum</trd>
  <trdgrp lng="nl">
    <trd>glasachtig lichaam</trd>,
    <trd>corpus vitreum</trd>
  </trdgrp>
  <trd lng="sk">sklivec</trd>
  <trd lng="uk">&c_s;&#x43A;&#x43B;&#x43E;&c_v;&c_i;&c_d;&#x43D;&c_je; &c_t;&c_ib;&#x43B;&#x43E;</trd>
</drv>
</art>

