<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>kosmopolit</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kosmop.0o">
  <kap><tld/>o</kap>
  <snc mrk="kosmop.0o.mondcivitano">
    <ref tip="dif" cel="civit.mond0ano">mondcivitano</ref>
    <trd lng="hu">vil&aacute;gpolg&aacute;r</trd>    
  </snc>
  <trd lng="pt">cosmopolita <klr tip="amb">(subst.)</klr></trd>
  <snc mrk="kosmop.0o.vojagxa">
    <dif>
      Homo, ordinare ri&ccirc;a kaj monduma, kiu pasigas sian vivda&ubreve;ron
      vo&jcirc;a&gcirc;ante tra diversaj landoj:
      <ekz>
        <tld/>a aventuristo.
      </ekz>
    </dif>
    <trd lng="hu">kozmopolita</trd>    
  </snc>
  <trd lng="be">&c_k;&c_a;&c_s;&c_m;&c_a;&c_p;&c_a;&c_l;&c_ib;&c_t;</trd>
  <trd lng="ca">cosmopolita</trd>
  <trdgrp lng="de">
     <trd>Weltb&uuml;rger</trd>,
     <trd>Kosmopolit</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cosmopolitan</trd>,
    <trd>cosmopolite</trd>
  </trdgrp>
  <trd lng="es">cosmopolita</trd>
  <trdgrp lng="fr">
    <trd>cosmopolite<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="hu">vil&aacute;gpolg&aacute;r</trd>  
  <trd lng="ru">&c_k;&c_o;&c_s;&c_m;&c_o;&c_p;&c_o;&c_l;&c_i;&c_t;</trd>
</drv>

<drv mrk="kosmop.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="kosmop.0eco.POL">
    <ref tip="dif" cel="civit.mond0aneco">mondcivitaneco</ref>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_s;&c_m;&c_a;&c_p;&c_a;&c_l;&c_ib;&c_t;&c_y;&c_z;&c_m;</trd>
  <trdgrp lng="de">
     <trd>Weltb&uuml;rgertum</trd>,
     <trd>Kosmopolitentum</trd>
  </trdgrp>
  <trd lng="en">cosmopolitanism</trd>
  <trd lng="fr">cosmopolitisme</trd>
  <trdgrp lng="hu">
     <trd>vil&aacute;gpolg&aacute;rs&aacute;g <klr tip="ind">(mint &aacute;llapot)</klr></trd>,
     <trd>kozmopolitizmus</trd>
  </trdgrp>  
  <trd lng="ru">&c_k;&c_o;&c_s;&c_m;&c_o;&c_p;&c_o;&c_l;&c_i;&c_t;&c_i;&c_z;&c_m;</trd>
</drv>

<drv mrk="kosmop.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="kosmop.0ismo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Sento de aparteno al monda socio,
      prefere ol al aparta gento, kaj
      al tio konforma sinteno;
      <ref tip="sin" cel="naci.inter0ismo.POL">internaciismo</ref>. 
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_s;&c_m;&c_a;&c_p;&c_a;&c_l;&c_ib;&c_t;&c_y;&c_z;&c_m;</trd>
  <trd lng="ca">cosmopolitisme</trd>
  <trdgrp lng="de">
     <trd>Weltb&uuml;rgerschaft</trd>,
     <trd>Kosmopolitismus</trd>
  </trdgrp>
  <trd lng="en">cosmopolitanism</trd>
  <trd lng="es">cosmopolitismo</trd>
  <trd lng="fr">cosmopolitisme</trd>
  <trd lng="hu">kozmopolitizmus</trd>  
  <trd lng="ru">&c_k;&c_o;&c_s;&c_m;&c_o;&c_p;&c_o;&c_l;&c_i;&c_t;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
