<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>kosmopolit</rad>/o
</kap>
<drv mrk="kosmop.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Homo, kiu rigardas la tutmondon kiel sian patrujon; mondcivitano,
      homarano.
    </dif>
  </snc>
  <snc>
    <dif>
      Homo, ordinare ri&ccirc;a kaj monduma, kiu pasigas sian vivda&ubreve;ron
      vo&jcirc;a&gcirc;ante tra diversaj landoj:
      <ekz>
        <tld/>a aventuristo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>cosmopolitan</trd>,
    <trd>cosmopolite</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_m;&c_o;&c_p;&c_o;&c_l;&c_i;&c_t;</trd>
</drv>

<drv mrk="kosmop.0eco0ismo">
  <kap>
    <tld/>eco, <tld/>ismo
  </kap>
  <snc>
    <dif>
      Doktrino kaj vivmaniero de <tld/>o
    </dif>
    <refgrp tip="sin">
      <ref cel="naci.sen0ismo">sennaciismo</ref>,
      <ref cel="hom.0aranismo">homaranismo</ref>.
    </refgrp>
  </snc>
  <trd lng="en">cosmopolitanism</trd>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_m;&c_o;&c_p;&c_o;&c_l;&c_i;&c_t;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
