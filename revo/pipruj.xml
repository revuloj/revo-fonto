<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pipruj.xml,v 1.11 2022/03/26 10:43:37 revo Exp $">
<kap>
  <rad>piprujac</rad>/oj,
  <var><kap><rad>piperac</rad>/oj</kap></var>
</kap>
<drv mrk="pipruj.0oj">
  <kap><tld/>oj,
    <var><kap>piperacoj</kap></var>
  </kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Piperaceae</trd>):
      <ekz>
        la <tld/>oj a&ubreve; piperacoj <klr>[&#x2026;]</klr> kun senperiantaj floroj,
        kaj
        kun fruktoj kapsulecaj a&ubreve; berecaj, estas &ccirc;. 3000 specioj en 8
        genroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=24463">Piprujacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
       <ref cel="pipr.0ujo.genro">piprujo</ref>,
       <ref cel="betel.0o">betelo</ref>    
    </refgrp>
  </snc>
  <trd lng="de">Pfeffergew&auml;chse</trd>
  <trdgrp lng="en"><trd>Piperaceae</trd>, <trd>pepper
  family</trd></trdgrp>
  <trd lng="fr">Pip&eacute;rac&eacute;es</trd>
  <trd lng="nl">piperaceae</trd>
  <trd lng="pl">pieprzowate</trd>
  <trd lng="pt">piper&aacute;ceas</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_ch;&c_n;&c_y;&c_je;<klr tip="ind">
    (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_p;&c_je;&c_r;&c_c;&c_je;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x435;&#x440;&#x446;&#x435;&#x432;&#x456;</trd>
</drv>
</art>

