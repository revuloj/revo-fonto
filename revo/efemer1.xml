<art mrk="$Id: efemer1.xml,v 1.17 2022/03/16 15:54:27 revo Exp $">
<kap>
  <rad>efemerid</rad>/oj
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="efemer1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Astronomiaj tabeloj sciigantaj la pozicion de planedoj por &ccirc;iu
      tago de la jaro:
      <ekz>
        la talenton de <nom>Stieler</nom> desegni mapojn malkovris 
        <nom>J&aacute;nos Ferenc Z&aacute;ch</nom>, la estro de la stelobservejo <nom>Seeberg</nom>: 
        por ties <tld lit="E"/>oj kaj Monataj korespondadoj <nom>Stieler</nom>
        kelkfoje desegnis mapetojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=507450">Adolf Stieler</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_f;&c_e;&c_m;&c_e;&c_r;&c_y;&c_d;&c_y;</trd>
  <trd lng="cs">efemeridy</trd>
  <trd lng="de">Ephemeriden</trd>
  <trd lng="es">ef&eacute;merides</trd>
  <trdgrp lng="fa">
  <trd>&a_t;&a_q;&a_w;&f_y;&a_m; &a_n;&a_j;&a_w;&a_m;&f_y;</trd>,
  <trd>&a_s;&a_A;&a_l;&a_n;&a_m;&a_A;&f_y; &a_n;&a_j;&a_w;&a_m;&f_y;</trd>
</trdgrp>
  <trd lng="fr">&eacute;ph&eacute;m&eacute;rides<klr tip="ind"> (table
astronomique)</klr></trd>
  <trd lng="hu">efemerid&aacute;k</trd>
  <trd lng="nl">efemeriden</trd>
  <trd lng="pl">efemerydy <klr> (astronomia)</klr></trd>
  <trd lng="pt">efem&eacute;rides</trd>
  <trd lng="ru">&c_e;&c_f;&c_je;&c_m;&c_je;&c_r;&c_i;&c_d;&c_y;</trd>
  <trd lng="sk">efemeridy</trd>
  <trd lng="uk">&#x435;&#x444;&#x435;&#x43C;&#x435;&#x440;&#x438;&#x434;&#x438;</trd>
</drv>
