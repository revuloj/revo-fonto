<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>korn</rad>/o
</kap>
<drv mrk="korn.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Longa konusa malmola elstara&jcirc;o, rekta a&ubreve; kurba, kiu kreskas sur
      frunto a&ubreve; alia parto de kapo de rema&ccirc;uloj (precipe viraj):
      <ekz>
        <tld/>o de vir&scirc;afo, rinocero;
      </ekz>
      <ekz>
        preni la bovon per la <tld/>oj<fnt>Z</fnt>
        <klr>(f. iri rekte kaj decide al la plej malfacila afero)</klr>;
      </ekz>
      <ekz>
        <tld/>obati, <tld/>ofrapi;
      </ekz>
      <ekz>
        <tld/>havaj bestoj.
      </ekz>
    </dif>
    <trd lng="de">Horn</trd>
    <trd lng="ru">&c_r;&c_o;&c_g;</trd>
  </snc>
  <snc>
    <dif>
      <tld lit="H"/>a substanco.
    </dif>
    <trd lng="de">Horn</trd>
  </snc>
  <snc>
    <dif>
      Bova <tld/>o, uzata kiel ujo a&ubreve; <ref tip="vid"
      cel="instru2.muzik0o">muzikinstrumento</ref>:
      <ekz>
        li prenis la <tld/>on kun oleo kaj sanktoleis
        Salomonon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        trinki, blovi el <tld/>o.
      </ekz>
    </dif>
    <trd lng="de">Horn</trd>
    <trd lng="ru">&c_r;&c_o;&c_g;</trd>
  </snc>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Speco de muzika <ref tip="super"
      cel="instru2.blov0o">blovinstrumento</ref>.
    </dif>
    <refgrp tip="sin">
      <ref cel="korn.cxas0o">&ccirc;askorno</ref>
    </refgrp>
    <trd lng="de">Horn</trd>
    <trd lng="ru">&c_r;&c_o;&c_zh;&c_o;&c_k;</trd>
  </snc>
  <snc>
    <uzo tip="fak">BIB</uzo>
    <dif>
      Potenco kaj fiero:
      <ekz>
        la nacioj levis sian <tld/>on kontra&ubreve; Izrael<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li altigis la <tld/>ojn de viaj kontra&ubreve;uloj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li rompis &ccirc;iujn <tld/>ojn de Izrael<fnt>Z</fnt>;
      </ekz>
      <ekz>
        dehaki la <tld/>ojn de la nacioj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">hoorn</trd>
</drv>

<drv mrk="korn.0eca">
  <kap>
    <tld/>eca
  </kap>
  <snc>
    <dif>
      Farita el substanco simila al <tld/>o:
      <ekz>
        <tld/>eca kombilo, tabakujo.
      </ekz>
    </dif>
  </snc>
  <trd lng="ru">&c_r;&c_o;&c_g;&c_o;&c_v;&c_o;&c_j;</trd>
</drv>

<drv mrk="korn.0aro0obrancxaro">
  <kap>
    <tld/>aro, <tld/>obran&ccirc;aro
  </kap>
  <snc>
    <dif>
      Bran&ccirc;formaj <tld/>oj de cervo kaj samspecaj bestoj.
    </dif>
  </snc>
  <trd lng="nl">hoornachtig</trd>
  <trd lng="ru">&c_r;&c_o;&c_g;&c_a; <klr>(&c_v;&c_je;&c_t;&c_v;&c_i;&c_s;&c_t;&c_y;&c_je;)</klr></trd>
</drv>

<drv mrk="korn.cxas0o">
  <kap>
    &ccirc;as<tld/>o
  </kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Kupra <ref tip="super" cel="instru2.blov0o">blovinstrumento</ref>,
      spiralforma, kies unu ekstrema&jcirc;o estas garnita per bu&scirc;ingo, kaj alia
      formas funelforman konuson.
    </dif>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_o;&c_h;&c_o;&c_t;&c_n;&c_i;&c_ch;&c_i;&c_j; &c_r;&c_o;&c_zh;&c_o;&c_k;</trd>,
    <trd>&c_r;&c_o;&c_g;</trd>
  </trdgrp>
</drv>

<drv mrk="korn.paper0eto">
  <kap>
    paper<tld/>eto<fnt>Z</fnt>
  </kap>
  <snc>
    <dif>
      Pinta papersaketo.
    </dif>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_r;&c_o;&c_zh;&c_o;&c_k; <klr>(&c_b;&c_u;&c_m;&c_a;&c_zh;&c_n;&c_y;&c_j;)</klr></trd>,
    <trd>&c_k;&c_u;&c_l;&c_jo;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="korn.sxu0o">
  <kap>
    &scirc;u<tld/>o
  </kap>
  <snc>
    <dif>
      Duono de <tld/>o, segita la&ubreve; la longo, kies lar&gcirc;a parto servas por
      glitigi kalkanon en &scirc;uon.
    </dif>
  </snc>
  <trdgrp lng="nl">
    <trd>schoenlepel</trd>,
    <trd>schoentrekker</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_o;&c_zh;&c_o;&c_k; <klr>(&c_d;&c_l;&c_ja; &c_o;&c_b;&c_u;&c_v;&c_i;)</klr></trd>
</drv>

<drv mrk="korn.unu0ulo">
  <kap>
    unu<tld/>ulo
  </kap>
  <snc>
    <dif>
      Besto kun unu <tld/>o.
    </dif>
  </snc>
  <trd lng="nl">eenhoorn</trd>
</drv>

<drv mrk="korn.du0ulo">
  <kap>
    du<tld/>ulo
  </kap>
  <snc>
    <dif>
      Besto kun du <tld/>oj.
    </dif>
  </snc>
  <trd lng="nl">tweehoorn</trd>
</drv>
</art>
