      kaj plue tiel nomata:
      <ekz>
        eta &leftquot;<nom>PEE</nom>&rightquot; de a&ubreve;to<tld/>o altiris lian atenton
        <fnt>
          <aut>S. Rodin</aut>
          <vrk>Bildoj pri Norda Lando</vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_ja;&c_k;&c_s;&c_a;&c_n;</trd>
  <trd lng="de">Hupe</trd>
  <trdgrp lng="fr">
    <trd>avertisseur</trd>,
    <trd>klaxon</trd>
  </trdgrp>
  <trd lng="id">klakson</trd>
  <trdgrp lng="it">
    <trd>tromba<klr> (di automobile)</klr></trd>,
    <trd>clacson</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>claxon</trd>,
    <trd>toeter</trd>
  </trdgrp>
  <trd lng="pl">klakson</trd>
  <trd lng="pt">buzina<klr> de autom&oacute;vel</klr></trd>
  <trd lng="ro">claxon</trd>
  <trd lng="uk">&c_s;&c_i;&c_gu;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j; &c_r;&c_ib;&c_zh;&c_o;&c_k;</trd>
</drv>

<drv mrk="korn.cxas0o">
  <kap>&ccirc;as<tld/>o</kap>
  <snc mrk="korn.cxas0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      E&ubreve;ropa metala <ref tip="lst" cel="instru2.blov0o"
      lst="voko:blovinstrumentoj">blovinstrumento</ref>
      kun longa, cirkle volvita tubo kaj vasta klo&scirc;o,
      tradicie uzata por signaloj dum &ccirc;asado:
      <ekz>
        jen inter la montoj eka&ubreve;di&gcirc;is la sonoj de &ccirc;as<tld/>o
        <fnt><bib>Fab1</bib>, <lok>sova&gcirc;aj cignoj</lok></fnt>;
      </ekz>
      <ekz>
        la bruo de &ccirc;as<tld/>oj <klr>[&#x2026;]</klr> ilin tute erarigis
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;as<tld/>o sonis, kaj la hundoj bojis
        <fnt>
          <bib>Fab4</bib>
