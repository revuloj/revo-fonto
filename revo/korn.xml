<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>korn</rad>/o
</kap>
<drv mrk="korn.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="korn.0o.besta">
    <dif>
      Malmola elstara&jcirc;o, kiu kreskas sur
      frunto a&ubreve; alia kapo-parto de bestoj, precipe de viraj
      rema&ccirc;uloj:
      <ekz>
        <tld/>o de vir&scirc;afo, rinocero;
      </ekz>
      <ekz>
        preni la bovon per la <tld/>oj<fnt>Z</fnt>
        <klr>(f. iri rekte kaj decide al la plej malfacila afero)</klr>;
      </ekz>
      <ekz>
        <tld/>obati, <tld/>ofrapi;
      </ekz>
      <ekz>
        <tld/>havaj bestoj.
      </ekz>
    </dif>
    <trd lng="de">Horn</trd>
    <trd lng="en">horn</trd>
    <trdgrp lng="fr">
      <trd>corne</trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_o;&c_g;</trd>
  </snc>
  <snc mrk="korn.0o.ajxo">
    <dif>
      Bova <tld/>o a&ubreve; alia simile konusa a&jcirc;o uzata kiel ujo:
      <ekz>
        li prenis la <tld/>on kun oleo kaj sanktoleis
        Salomonon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        trinki, blovi el <tld/>o.
      </ekz>
    </dif>
    <trd lng="de">Horn</trd>
    <trd lng="en">horn</trd>
    <trdgrp lng="fr">
      <trd>corne<klr> (objet)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_o;&c_g;</trd>
  </snc>
  <snc mrk="korn.0o.substanco">
    <dif>
      <tld lit="K"/>a substanco.
    </dif>
    <trd lng="de">Horn</trd>
    <trd lng="en">horny</trd>
    <trdgrp lng="fr">
      <trd>corne <klr> (mati&egrave;re)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="korn.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Besta korno truita &ccirc;e la pinto a&ubreve; &ccirc;e flanko,
      uzata kiel <ref tip="vid" cel="instru2.blov0o">blovinstrumento</ref>
      populara en diversaj mondpartoj.
    </dif>
    <trdgrp lng="fr">
      <trd>cor<klr> (corne utilis&eacute;e en musique)</klr></trd>,
      <trd>trompe<klr> (musique)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_r;&c_o;&c_g;</trd>,
      <trd>&c_r;&c_o;&c_zh;&c_o;&c_k;<klr>
(&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;
&c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="korn.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Simbolo de potenco kaj fiero:
      <ekz>
        la nacioj levis sian <tld/>on kontra&ubreve; Izrael<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li altigis la <tld/>ojn de viaj kontra&ubreve;uloj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li rompis &ccirc;iujn <tld/>ojn de Izrael<fnt>Z</fnt>;
      </ekz>
      <ekz>
        dehaki la <tld/>ojn de la nacioj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">cuerno</trd>
  <trd lng="nl">hoorn</trd>
</drv>

<drv mrk="korn.0eca">
  <kap><tld/>eca</kap>
  <snc>
    <dif>
      Farita el substanco simila al <tld/>o:
      <ekz>
        <tld/>eca kombilo, tabakujo.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">horny</trd>
  <trdgrp lng="fr">
    <trd>corn&eacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_o;&c_g;&c_o;&c_v;&c_o;&c_j;</trd>
</drv>

<drv mrk="korn.0aro0obrancxaro">
  <kap><tld/>aro, <tld/>obran&ccirc;aro</kap>
  <snc>
    <dif>
      Bran&ccirc;formaj <tld/>oj de <ref tip="vid"
      cel="cerved.0oj">cervedoj</ref>.
    </dif>
  </snc>
  <trd lng="nl">hoornachtig</trd>
  <trd lng="en">antlers</trd>
  <trdgrp lng="fr">
    <trd>andouiller</trd>,
    <trd>bois<klr> (de cervid&eacute;)</klr></trd>,
    <trd>cors<klr> (de cervid&eacute;)</klr></trd>,
    <trd>massacre<klr> (de cervid&eacute;)</klr></trd>,
    <trd>ramure<klr> (de cervid&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_o;&c_g;&c_a;<klr>
(&c_v;&c_je;&c_t;&c_v;&c_i;&c_s;&c_t;&c_y;&c_je;)</klr></trd>
</drv>

<drv mrk="korn.cxas0o">
  <kap>&ccirc;as<tld/>o</kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      E&ubreve;ropa metala <ref tip="super"
      cel="instru2.blov0o">blovinstrumento</ref>
      kun longa, cirkle volvita tubo kaj vasta klo&scirc;o.
    </dif>
  </snc>
  <trd lng="de">Horn</trd>
  <trd lng="en">hunting horn</trd>
  <trdgrp lng="fr">
    <trd>cor<klr> (d'orchestre)</klr></trd>,
    <trd>cor de chasse</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_h;&c_o;&c_t;&c_n;&c_i;&c_ch;&c_i;&c_j;
&c_r;&c_o;&c_zh;&c_o;&c_k;</trd>,
    <trd>&c_r;&c_o;&c_g;<klr>
(&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;
&c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="korn.paper0eto">
  <kap>paper<tld/>eto<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Pinta papersaketo.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>cornet de papier</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_o;&c_zh;&c_o;&c_k;<klr>
(&c_b;&c_u;&c_m;&c_a;&c_zh;&c_n;&c_y;&c_j;)</klr></trd>,
    <trd>&c_k;&c_u;&c_l;&c_jo;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="korn.sxu0o">
  <kap>&scirc;u<tld/>o</kap>
  <snc>
    <dif>
      La&ubreve;longa duono de <tld/>o a&ubreve; simila duonfunela ilo
      por faciligi engliton de kalkano en &scirc;uon.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>shoe horn</trd>,
    <trd>shoe spoon</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>corne &agrave; chaussure</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schoenlepel</trd>,
    <trd>schoentrekker</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_o;&c_zh;&c_o;&c_k;<klr> (&c_d;&c_l;&c_ja;
&c_o;&c_b;&c_u;&c_v;&c_i;)</klr></trd>
</drv>

<drv mrk="korn.unu0ulo">
  <kap>unu<tld/>ulo</kap>
  <snc>
    <dif>
      Besto kun unu <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>licorne</trd>,
    <trd>unicorne</trd>
  </trdgrp>
  <trd lng="nl">eenhoorn</trd>
  <trd lng="ru">&c_je;&c_d;&c_i;&c_n;&c_o;&c_r;&c_o;&c_g;</trd>
</drv>

<drv mrk="korn.du0ulo">
  <kap>du<tld/>ulo</kap>
  <snc>
    <dif>
      Besto kun du <tld/>oj.
    </dif>
  </snc>
  <trd lng="nl">tweehoorn</trd>
</drv>
</art>
