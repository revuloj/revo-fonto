    <rad>endokrinolog</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="endokrinolog.0o">
  <kap><tld/>o</kap>
  <snc mrk="endokrinolog.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="kurac.0isto">Kuracisto</ref>, kies fako
      estas <ref tip="vid" cel="endokrinologi.0o"><tld/>io</ref>:
      <ekz>
        en la &ccirc;efurbo necesas atendi duonjaron por viziti <tld/>on
        <fnt>
          <aut>S; Belov</aut>,
          <vrk><url
          ref="http://stanobelov.blogspot.com/2018/12/kubo-mito-pri-la-bona-medicino.html">
          Kubo &ndash; mito pri la bona medicino </url></vrk>,
          <lok>2018-12-30</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_n;&c_d;&c_a;&c_k;&c_r;&c_y;&c_n;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="en">endocrinologist</trd>
  <trd lng="fr">endocrinologue</trd>
  <trd lng="uk">&c_je;&#x43D;&c_d;&#x43E;&#x43A;&c_r;&c_i;&#x43D;&#x43E;&#x43B;&#x43E;&c_g;</trd>
</drv>
