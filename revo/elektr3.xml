<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: elektr3.xml,v 1.23 2023/11/07 20:34:10 revo Exp $">
<kap>
  <rad>elektrod</rad>/o
</kap>
<drv mrk="elektr3.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ELE</uzo>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Peco el <ref cel="konduk.0anto.FIZ_ELE">kondukanto</ref>,
      kapabla transirigi elektron de &gcirc;i al &ccirc;irka&ubreve;anta
      &gcirc;in fluida&jcirc;o, kaj inverse.
      <ekz>
        ekzistas du <tld/>oj: 
        <ref tip="sub" cel="katod.0o">katodo</ref> (negativa) kaj 
        <ref tip="sub" cel="anod.0o">anodo</ref> (pozitiva).
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_d;</trd>
  <trd lng="bg">&c_je;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_d;</trd>
  <trd lng="cs">elektroda</trd>
  <trd lng="de">Elektrode</trd>
  <trd lng="en">electrode</trd>
  <trd lng="es">electrodo</trd>
  <trd lng="fr">&eacute;lectrode</trd>
  <trd lng="hu">elektr&oacute;d</trd>
  <trd lng="ja">&#x96FB;&#x6975;  <pr>&#x3067;&#x3093;&#x304D;&#x3087;&#x304F;</pr></trd>
  <trd lng="pl">elektroda</trd>
  <trdgrp lng="pt">
    <trd>el&eacute;trodo</trd>,
    <trd>eletrodo</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_d;</trd>
  <trd lng="sk">elektr&#xF3;da</trd>
  <trd lng="uk">&#x435;&#x43B;&#x435;&#x43A;&#x442;&#x440;&#x43E;&#x434;</trd>
  <trd lng="zh">&#x7535;&#x6781; <pr>di&#xE0;nj&#xED;</pr></trd>
</drv>
</art>
