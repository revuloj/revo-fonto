<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hipohx.xml,v 1.29 2023/11/15 17:08:01 revo Exp $">
<kap>
  <rad>hipo&hcirc;ondri</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
  <var><kap><rad var="k">hipokondri</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>
<drv mrk="hipohx.0o">
  <kap><tld/>o,
    <var><kap><tld var="k"/>o</kap></var>
  </kap>
  <snc mrk="hipohx.0o.PSI">
    <uzo tip="fak">MED</uzo>
    <uzo tip="fak">PSI</uzo>
    <dif>
      Nerva <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>, karakterizata per
      malgaja stato
      de la animo kun senka&ubreve;za timo pri la sano:
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> priskribas konvinkon de personoj havi certajn severajn
        korpajn malsanojn, kaj la timegon de tiuj personoj pri malsani&gcirc;o, kvankam
        medicine tiaj korpaj malsanoj &ccirc;e la pacientoj neniel konfirmeblas, esence
        temas pri specifa <klr>[&#x2026;]</klr> paranojo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=244232">Hipo&hcirc;ondrio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        Galeno <klr>[&#x2026;]</klr> estis la unua, kiu sisteme priskribis la psikosomatikan
        malsanon <tld lit="H"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=244311">Galeno de Pergamo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        lia <tld var="k"/>o trovi&gcirc;is en stranga kontrasto al lia forto
        <fnt>
          <aut>trad. Donjo, trad. Cezar</aut>,
          <vrk><url ref="http://karapacopanoramo.blogspot.com/2008/04/spitzweg-bidermajra-honestulo-kaj.html">
          Spitzweg - Bidermajra honestulo kaj revoluciulo</url></vrk>,
          <lok>2008-04-28</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ctl><tld var="k"/>o estas la sola malsano, pro kiu mi ne suferas</ctl>
        <fnt>
          <aut>trad. P. Thorsen</aut>,
          <vrk><url ref="http://www.poulthorsen.dk/grafit.htm">
           Grafita&jcirc;oj</url></vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_p;&c_a;&c_h;&c_o;&c_n;&c_d;&c_r;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_h;&c_i;&c_p;&c_o;&c_h;&c_o;&c_n;&c_d;&c_r;&c_i;&c_ja;</trd>
  <trd lng="ca">hipocondria</trd>
  <trd lng="de">Hypochondrie</trd>
  <trd lng="el">&ypsilon;&pi;&omikron;&chi;&omikron;&ny;&delta;&rho;&jota_ton;&alfa;</trd>
  <trd lng="en">hypochondria</trd>
  <trd lng="es">hipocondr&iacute;a</trd>
  <trd lng="fr">hypocondrie</trd>
  <trdgrp lng="hu">
    <trd>k&eacute;pzelt <ind>betegs&eacute;g</ind></trd>,
    <trd>hipochondria</trd>
  </trdgrp>
  <trd lng="id">hipokondria</trd>
  <trd lng="it">ipocondria</trd>
  <trdgrp lng="ja">
    <trd>&#x5FC3;&#x6C17;&#x75C7;  <pr>&#x3057;&#x3093;&#x304D;&#x3057;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x30D2;&#x30DD;&#x30B3;&#x30F3;&#x30C7;&#x30EA;&#x30FC;</trd>
  </trdgrp>
  <trd lng="nl">hypochondrie</trd>
  <trd lng="pl">hipochondria</trd>
  <trd lng="pt">hipocondria</trd>
  <trd lng="ru">&c_i;&c_p;&c_o;&c_h;&c_o;&c_n;&c_d;&c_r;&c_i;&c_ja;</trd>
  <trdgrp lng="uk">
    <trd>&c_ib;&#x43F;&#x43E;&c_h;&#x43E;&#x43D;&c_d;&c_r;&c_ib;&#x44F;</trd>,
    <trd>&#x43F;&c_r;&c_i;&c_g;&#x43D;&c_ib;&c_ch;&c_je;&#x43D;&c_i;&c_j; &c_s;&c_t;&c_a;&#x43D; &c_d;&c_u;&c_h;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7591;&#x75C5;&#x75C7; <pr>y&#xED;b&#xEC;ngzh&#xE8;ng</pr></trd>,
    <trd>&#x616E;&#x75C5;&#x75C7; <pr>l&#xF9;:b&#xEC;ngzh&#xE8;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hipohx.0ulo">
  <kap><tld/>ulo,
    <var><kap><tld var="k"/>ulo</kap></var>
  </kap>
  <snc mrk="hipohx.0ulo.PSI">
    <dif>
      Homo, suferanta la <tld/>on:
      <ekz>
        la malnova <tld/>ulo &ccirc;i tie sen&ccirc;ese &gcirc;emadas
        <fnt>
          <aut>trad. M. Blahu&scirc;</aut>,
          <vrk><url ref="http://filmoj.net/kusxejetoj">
          Ku&scirc;ejetoj</url></vrk>,
          <lok>[2009]</lok>
        </fnt>!
      </ekz>
      <ekz>
        mi nutris svagan esperon, ke la ekscito nun agitanta la
        <tld var="k"/>ulon eble trovos mildi&gcirc;on
        <fnt>
          <aut>E. A. Poe, trad. E. Grimley-Evans</aut>,
          <vrk><url ref="&LOdE;Tl-ang07.htm#Tradukinto">
          La falo de la domo Usher</url></vrk>,
          <lok>Sezonoj, 2002</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_p;&c_a;&c_h;&c_o;&c_n;&c_d;&c_r;&c_y;&c_k;</trd>
  <trd lng="bg">&c_h;&c_i;&c_p;&c_o;&c_h;&c_o;&c_n;&c_d;&c_r;&c_i;&c_k;</trd>
  <trd lng="ca">hipocondr&iacute;ac</trd>
  <trd lng="de">Hypochonder</trd>
  <trd lng="el">&ypsilon;&pi;&omikron;&chi;&omikron;&ny;&delta;&rho;&jota;&alfa;&kappa; &omikron_ton;&sigma_fina;</trd>
  <trd lng="en">hypochondriac</trd>
  <trd lng="es">hipocondriaco</trd>
  <trd lng="fr">hypocondriaque<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>k&eacute;pzelt <ind>beteg</ind></trd>,
    <trd>hipochonder</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>ipocondriaco</trd>,
    <trd><ind>malato</ind> immaginario</trd>
  </trdgrp>
  <trd lng="ja">&#x5FC3;&#x6C17;&#x75C7;&#x60A3;&#x8005;  <pr>&#x3057;&#x3093;&#x304D;&#x3057;&#x3087;&#x3046;&#x304B;&#x3093;&#x3058;</pr></trd>
  <trdgrp lng="nl">
    <trd>hypochonder</trd>,
    <trd>hypochondrist</trd>
  </trdgrp>
  <trd lng="pl">hipochondryk</trd>
  <trd lng="pt">hipocondr&iacute;aco</trd>
  <trd lng="ru">&c_i;&c_p;&c_o;&c_h;&c_o;&c_n;&c_d;&c_r;&c_i;&c_k;</trd>
  <trd lng="uk">&c_ib;&#x43F;&#x43E;&c_h;&#x43E;&#x43D;&c_d;&c_r;&c_i;&#x43A;</trd>
  <trd lng="zh">&#x7591;&#x75C5;&#x75C7;&#x60A3;&#x8005; <pr>y&#xED;b&#xEC;ngzh&#x113;nghu&#xE0;nzh&#x11B;</pr></trd>
</drv>
</art>
