<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hipo&hcirc;ondri</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="hipohx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Nerva malsano, karakterizata per malgaja stato de la animo
      kun senka&ubreve;za timo pri la sano.
    </dif>
  </snc>
  <trd lng="de">Hypochondrie</trd>
  <trd lng="el">&ypsilon;&pi;&omikron;&chi;&omikron;&ny;&delta;&rho;&jota_ton;&alfa;</trd>
  <trd lng="fr">hypocondrie</trd>
  <trd lng="nl">hypochondrie</trd>
  <trd lng="pt">hipocondria</trd>
  <trd lng="ru">&c_i;&c_p;&c_o;&c_h;&c_o;&c_n;&c_d;&c_r;&c_i;&c_ja;</trd>
</drv>

<drv mrk="hipohx.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Homo, suferanta la <tld/>on.
    </dif>
  </snc>
  <trd lng="de">Hypochonder</trd>
  <trd lng="el">&ypsilon;&pi;&omikron;&chi;&omikron;&ny;&delta;&rho;&jota;&alfa;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trd lng="fr">hyponcondriaque<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="nl">
    <trd>hypochonder</trd>,
    <trd>hypochondrist</trd>
  </trdgrp>
  <trd lng="pt">hipocondr&iacute;aco</trd>
  <trd lng="ru">&c_i;&c_p;&c_o;&c_h;&c_o;&c_n;&c_d;&c_r;&c_i;&c_k;</trd>
</drv>
</art>
