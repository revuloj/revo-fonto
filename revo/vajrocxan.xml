<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vajrocxan.xml,v 1.6 2024/04/13 22:21:55 revo Exp $">
<kap>
  <rad>Vajro&ccirc;an</rad>/o
</kap>
<drv mrk="vajrocxan.0o">
  <kap><tld/>o</kap>
  <snc mrk="vajrocxan.0o.BUD">
    <uzo tip="fak">BUD</uzo>
    <dif>
      Iu el la &ccirc;efaj meditaj <ref tip="super" cel="buda.0o.nivelo">budaoj</ref> en va&gcirc;rajano:
      <ekz>
        Amitabo, <tld/>o kaj aliaj budhoj estis kreitaj de mahajanaj
        budhistoj
        <fnt>
          <aut>Yamaguti Sin'iti</aut>,
          <vrk><url
          ref="http://www001.upp.so-net.ne.jp/jble/art/miskompreno.html">
          Budhismo &Ccirc;irka&ubreve;ata de Miskompreno</url></vrk>,
          <lok>2006-01</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estas la simbolo de universala vero
        <fnt>
          <vrk><url ref="http://www001.upp.so-net.ne.jp/jble/movado/JB382.pdf"
              >La Japana Budhano</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="vajrocxan.0o.RIM_SIN">
    Aperas anka&ubreve; <em>Mahavajro&ccirc;ano</em>,
    tio estas &leftquot;granda <tld/>o&rightquot;
    <fnt><vrk><url
    ref="&UEA;vikio/Budhisma_Terminaro">Budhisma
    Terminaro</url></vrk>, <lok>2006-11</lok></fnt>.
  </rim>
  <trd lng="ca">Vairocana</trd>
  <trd lng="fr">Vairocana</trd>
  <trdgrp lng="hu">
    <trd>Vajrocsana</trd>,
    <trd>Virocsana</trd>
  </trdgrp>
  <trd lng="pl">Wajroczana</trd>
</drv>
</art>

