<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cefalotorak.xml,v 1.10 2022/02/25 09:30:23 revo Exp $">
<kap>
  <rad>cefalotorak</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="cefalotorak.0o">
  <kap><tld/>o</kap>
  <snc mrk="cefalotorak.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="torak.kap0o.ZOO">Kaptorako</ref>:
      <ekz>
        &ccirc;e longkruraj araneoj <tld/>o kaj abdomeno estas du apartaj 
        elementoj ligitaj per videbla, maldika tubo 
        <fnt>
          <aut>E. Nieuwenhuys, trad. L. De Cooman</aut>,
          <vrk><url
          ref="http://www.xs4all.nl/~ednieuw/Spiders/Pholcidae/PholcidaeEO.htm">Longkruruloj,
          svingaraneoj a&ubreve; keloaraneoj</url></vrk>,
          <lok>2006-03-10</lok>
        </fnt>;
      </ekz>
      <ekz>
        antenoj <klr>[&#8230;]</klr> estas segmentaj membroj situantaj sur la kapo a&ubreve; sur la
        <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=290872">Anteno (biologio)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_l;&c_a;&c_v;&c_a;&c_g;&c_r;&c_u;&c_d;&c_z;&c_ib;</trd>
  <trd lng="ca">cefalot&ograve;rax</trd>
  <trdgrp lng="cs">
    <trd>cefalotorax</trd>,
    <trd>hlavohru&dcaron;</trd>
  </trdgrp>
  <trd lng="de">Cephalothorax</trd>
  <trd lng="fr">c&eacute;phalothorax</trd>
  <trdgrp lng="it">
    <trd>cefalotorace</trd>,
    <trd>prosoma</trd>
  </trdgrp>
  <trd lng="en">cephalothorax</trd>
  <trd lng="pl">g&lstroke;owotu&lstroke;&oacute;w</trd>
  <trd lng="sk">cefalotorax</trd>
</drv>
</art>

