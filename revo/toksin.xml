<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>toksin</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="toksin.0o">
  <kap><tld/>o</kap>
  <snc mrk="toksin.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Baza <ref tip="malprt" cel="venen.0o.MED">venena</ref> 
      substanco nature produktata de iaj vivuloj: mikroboj, 
      vegeta&jcirc;oj, insektoj, serpentoj...:
      <ekz>
        la hepato akumulas <tld/>ojn
        <fnt>
          <aut>I. Cris&oacute;gno</aut>,
          <vrk><url 
          ref="http://www.geocities.com/HotSprings/4234/alkoholo.html">Alkoholismo</url></vrk>,
          <lok>[1996?]</lok>
        </fnt>;
      </ekz>
      <ekz>
        plantproduktoj enhavas multajn malutilajn elementojn kaj 
        <tld/>ojn
        <fnt>
          <aut>Zhong Q.</aut>,
          <vrk>&amp;#32032;&amp;#39135;&dash;&dash;&amp;#24858;&amp;#34850;&amp;#36824;&amp;#26159;&amp;#32874;&amp;#26126;&amp;#65311;</vrk>,
          <lok><url 
          ref="http://groups.google.be/group/chinese.text.unicode?hl=eo">chinese.text.unicode</url>,
          
          1996-07-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Toxine</trd>
  <trd lng="fr">toxine</trd>
  <trd lng="nl">toxine</trd>
  <trd lng="it">tossina</trd>
  <trd lng="pl">toksyna</trd>
</drv>
</art>

