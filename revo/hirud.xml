<art mrk="$Id: hirud.xml,v 1.21 2023/11/15 17:08:01 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>hirud</rad>/o
</kap>
<drv mrk="hirud.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el <ref tip="super" cel="verm.ringo0oj">ringovermoj</ref>
      (<trd lng="la">Hirudo</trd>),
      kiu su&ccirc;as la sangon de bestoj, kaj kies unu specion oni
      iam uzis en medicino por eltiri malgrandan kvanton da sango:
      <ekz>
        meti <tld/>ojn al iu;
      </ekz>
      <ekz>
        &gcirc;i helpos kiel <tld/>' al mortinto
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;'&c_ja;&c_w;&c_k;&c_a;</trd>
  <trd lng="bg">&c_p;&c_i;&c_ja;&c_v;&c_i;&c_c;&c_a;</trd>
  <trdgrp lng="br">         
    <trd>gelaouenn</trd>,
    <trd>gwaderez</trd>
  </trdgrp>
  <trd lng="cs">pijavice</trd>
  <trd lng="de">Blutegel</trd>
  <trd lng="el">&beta;&delta;&epsilon_ton;&lambda;&lambda;&alfa;</trd>
  <trd lng="en">leech</trd>
  <trd lng="fr">sangsue<klr tip="ind"> (zoo.)</klr></trd>
  <trd lng="hu">pi&oacute;ca</trd>
  <trd lng="it">sanguisuga</trd>
  <trd lng="ja">&#x30D2;&#x30EB;</trd>
  <trd lng="nl">bloedzuiger</trd>
  <trd lng="pl">pijawka</trd>
  <trd lng="pt">sanguessuga</trd>
  <trd lng="ru">&c_p;&c_i;&c_ja;&c_v;&c_k;&c_a;</trd>
  <trd lng="sk">pijavica</trd>
  <trd lng="uk">&#x43F;&#x2019;&#x44F;&#x432;&#x43A;&#x430;</trd>
  <trdgrp lng="zh">
    <trd>&#x86ED; <pr>zh&#xEC;</pr></trd>,
    <trd>&#x8682;&#x87E5; <pr>m&#x1CE;hu&#xE1;ng</pr></trd>,
    <trd>&#x6C34;&#x86ED; <pr>shu&#x1D0;zh&#xEC;</pr></trd>,
    <trd>&#x99AC;&#x9C49; <pr>m&#x1CE;bi&#x113;</pr></trd>,
    <trd>&#x5438;&#x8840;&#x8005; <pr>x&#x12B;xu&#xE8;zh&#x11B;</pr></trd>,
    <trd>&#x87E5; <pr>hu&#xE1;ng</pr></trd>,
    <trd>&#x5438;&#x8840;&#x866B; <pr>x&#x12B;xi&#x11B;ch&#xF3;ng</pr></trd>,
    <trd>&#x9AD8;&#x5229;&#x8CB8;&#x8005; <pr>g&#x101;ol&#xEC;d&#xE0;izh&#x11B;</pr></trd>,
    <trd>&#x69A8;&#x53D6;&#x4ED6;&#x4EBA;&#x91D1;&#x9322;&#x8005; <pr>zh&#xE0;q&#x1D4;t&#x101;r&#xE9;nj&#x12B;nqi&#xE1;nzh&#x11B;</pr></trd>
  </trdgrp>
</drv>
</art>
