<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>zigzag</rad>/o
</kap>
<drv mrk="zigzag.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Rompata linio, formanta alterne elstarajn kaj malelstarajn angulojn:
      <ekz>
        <tld/>a vojo;
      </ekz>
      <ekz>
        la fajraj fulmoj <tld/>e sin &jcirc;etadis en la &scirc;a&ubreve;man maron<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">zigzag</trd>
  <trd lng="be">&c_z;&c_ib;&c_g;&c_z;&c_a;&c_g;</trd>
  <trd lng="de">Zickzack</trd>
  <trd lng="es">zigzag</trd>
  <trdgrp lng="fr">
    <trd>zigzag</trd>
  </trdgrp>
  <trd lng="nl">zigzaglijn</trd>
  <trd lng="ru">&c_z;&c_i;&c_g;&c_z;&c_a;&c_g;</trd>
  <trd lng="sv">sicksacklinje</trd>
</drv>

<drv mrk="zigzag.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <tld lit="Z"/>e iri.
    </dif>
  <refgrp tip="vid">
    <ref cel="serpen.0i"></ref>
  </refgrp>
    </snc>
  <trd lng="be">&c_r;&c_u;&c_h;&c_a;&c_c;&c_c;&c_a; &c_z;&c_ib;&c_g;&c_z;&c_a;&c_g;&c_a;&c_m;</trd>
  <trd lng="en">zigzag</trd>
  <trd lng="es">zigzaguear</trd>
  <trdgrp lng="fr">
    <trd>zigzager</trd>
  </trdgrp>
  <trd lng="nl">zigzag lopen</trd>
</drv>
</art>
