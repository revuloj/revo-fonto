<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: zigzag.xml,v 1.28 2023/10/21 20:03:08 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>zigzag</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="zigzag.0o">
  <kap><tld/>o</kap>
  <snc mrk="zigzag.0o.FORM">
    <dif>
      Rompata linio, formanta alterne elstarajn kaj malelstarajn
      angulojn:
      <ekz>
        <tld/>a vojo;
      </ekz>
      <ekz>
        la fajraj fulmoj <tld/>e sin &jcirc;etadis en la &scirc;a&ubreve;man
        maron<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="meandr.0o.GEOG">meandro</ref>,
      <ref cel="serpen.0umo.forma">serpentumo</ref>,
      <ref cel="sinu.0o.FORM">sinuo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_ib;&c_g;&c_z;&c_a;&c_g;</trd>
  <trd lng="ca">ziga-zaga</trd>
  <trdgrp lng="cs">
    <trd>klikat&aacute; &ccaron;&aacute;ra</trd>,
    <trd>klikyh&aacute;k</trd>
  </trdgrp>
  <trd lng="de">Zickzack</trd>
  <trd lng="el">&zeta;&jota;&gamma;&kappa;-&zeta;&alfa;&gamma;&kappa;</trd>
  <trd lng="en">zigzag</trd>
  <trd lng="es">zigzag</trd>
  <trdgrp lng="fr">
    <trd>zigzag</trd>
  </trdgrp>
  <trd lng="hu">cikcakk</trd>
  <trd lng="it">zigzag</trd>
  <trdgrp lng="ja">
    <trd>&#x30B8;&#x30B0;&#x30B6;&#x30B0;</trd>,
    <trd>&#x7A32;&#x59BB;&#x5F62;  <pr>&#12356;&#x306A;&#x305A;&#x307E;&#x304B;&#x305F;</pr></trd>,
    <trd>&#xFF3A;&#x5B57;&#x5F62;  <pr>&#12380;&#12387;&#12392;&#12376;&#12369;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">zigzaglijn</trd>
  <trd lng="pt">ziguezague</trd>
  <trd lng="ru">&c_z;&c_i;&c_g;&c_z;&c_a;&c_g;</trd>
  <trdgrp lng="sk">
    <trd>krivolak&aacute; &ccaron;iara</trd>,
    <trd>k&lcaron;ukat&aacute;</trd>
  </trdgrp>
  <trd lng="sv">sicksacklinje</trd>
  <trd lng="uk">&c_z;&c_i;&c_gu;&c_z;&c_a;&c_gu;</trd>
</drv>

<drv mrk="zigzag.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="zigzag.0i.FORM">
    <dif>
      <tld lit="Z"/>e iri.
    </dif>
    <refgrp tip="vid">
      <ref cel="serpen.0umi.forme">serpentumi</ref>,
      <ref cel="sinu.0i.FORM">sinui</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_u;&c_h;&c_a;&c_c;&c_c;&c_a;
  &c_z;&c_ib;&c_g;&c_z;&c_a;&c_g;&c_a;&c_m;</trd>
  <trd lng="ca">zigzaguejar</trd>
  <trdgrp lng="cs">
    <trd>j&iacute;t klikat&ecaron;</trd>,
    <trd>klikatit se</trd>,
    <trd>kli&ccaron;kovat</trd>,
    <trd>pot&aacute;cet se</trd>,
    <trd>vr&aacute;vorat</trd>,
    <trd>&scaron;n&ecaron;rovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><klr>im </klr>Zickzack <ind>laufen</ind></trd>,
    <trd>herumzackern</trd>
  </trdgrp>
  <trd lng="el">&kappa;&alfa_ton;&ny;&omega; &zeta;&jota;&gamma;&kappa;-&zeta;&alfa;&gamma;&kappa;</trd>
  <trd lng="en">zigzag</trd>
  <trd lng="es">zigzaguear</trd>
  <trd lng="fr">zigzaguer</trd>
  <trd lng="hu">cik&aacute;zik</trd>
  <trd lng="it">zigzagare</trd>
  <trd lng="ja">&#x30B8;&#x30B0;&#x30B6;&#x30B0;&#x306B;&#36914;&#12416;  <pr>&#12376;&#12368;&#12374;&#12368;&#x306B;&#12377;&#12377;&#12416;</pr></trd>
  <trd lng="nl">zigzag lopen</trd>
  <trd lng="pt">ziguezaguear</trd>
  <trdgrp lng="sk">
    <trd>k&lcaron;u&ccaron;kova&tcaron;</trd>,
    <trd>pot&aacute;ca&tcaron; sa</trd>,
    <trd>&iacute;s&tcaron; k&lcaron;ukato</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&#x43E;&c_b;&c_i;&c_t;&c_i; &c_z;&c_i;&c_gu;&c_z;&c_a;&c_gu;&c_i;</trd>,
    <trd>&c_ib;&c_t;&c_i; &c_z;&c_i;&c_gu;&c_z;&c_a;&c_gu;&c_a;&#x43C;&c_i;</trd>,
    <trd>&c_v;&c_i;&#x43F;&c_i;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i; &#x201C;&#x43C;&c_i;&c_s;&#x43B;&c_ib;&c_t;&c_je;&#x201D;</trd>
  </trdgrp>
</drv>
</art>
