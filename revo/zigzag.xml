<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: zigzag.xml,v 1.32 2025/10/04 08:32:01 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>zigzag</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="zigzag.0o">
  <kap><tld/>o</kap>
  <snc mrk="zigzag.0o.FORM">
    <dif>
      Rompita <ref tip="super" cel="lini.0o">linio</ref>, alterne formanta
      angulon jen en unu flanko jen en la kontra&ubreve;a:
      <ekz>
        la fajraj fulmoj <tld/>e sin &jcirc;etadis en la &scirc;a&ubreve;man
        maron
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Drezen</nom> <klr>[&#x2026;]</klr> ur&gcirc;e rekomendis &ndash; por da&ubreve;rigi la kunlaboron &ndash; trovi mezan
        vojon por ni &ccirc;iuj, kun <tld/>oj jen al unu flanko, jen al alia
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li parkere konis &ccirc;iujn <tld/>ojn de la <klr>[&#x2026;]</klr> montaro
        <fnt>
          <bib>ChB</bib>
          <lok>2</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;is tiu punkto la trajno <klr>[&#x2026;]</klr> trapasis 29 pontojn, uzis 21 tunelojn,
        13 viaduktojn, 2 rondojn kaj 2 <tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Marta Sol&iacute;s</aut>
          <vrk><url ref="&Monato;/2005/008399.php"
              >Unika fervojo en Andoj</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="meandr.0o.GEOG">meandro</ref>,
      <ref cel="serpen.0umo.forma">serpentumo</ref>,
      <ref cel="sinu.0o.FORM">sinuo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_ib;&c_g;&c_z;&c_a;&c_g;</trd>
  <trd lng="ca">ziga-zaga</trd>
  <trdgrp lng="cs">
    <trd>klikat&aacute; &ccaron;&aacute;ra</trd>,
    <trd>klikyh&aacute;k</trd>
  </trdgrp>
  <trd lng="de">Zickzack</trd>
  <trd lng="el">&zeta;&jota;&gamma;&kappa;-&zeta;&alfa;&gamma;&kappa;</trd>
  <trd lng="en">zigzag</trd>
  <trd lng="es">zigzag</trd>
  <trdgrp lng="fr">
    <trd>zigzag</trd>
  </trdgrp>
  <trd lng="hu">cikcakk</trd>
  <trd lng="it">zigzag</trd>
  <trdgrp lng="ja">
    <trd>&#x30B8;&#x30B0;&#x30B6;&#x30B0;</trd>,
    <trd>&#x7A32;&#x59BB;&#x5F62;  <pr>&#x3044;&#x306A;&#x305A;&#x307E;&#x304B;&#x305F;</pr></trd>,
    <trd>&#xFF3A;&#x5B57;&#x5F62;  <pr>&#x305C;&#x3063;&#x3068;&#x3058;&#x3051;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">zigzaglijn</trd>
  <trdgrp lng="pl">
    <trd>zygzak</trd>,
    <trd>zakos</trd>
  </trdgrp>
  <trd lng="pt">ziguezague</trd>
  <trd lng="ru">&c_z;&c_i;&c_g;&c_z;&c_a;&c_g;</trd>
  <trdgrp lng="sk">
    <trd>krivolak&aacute; &ccaron;iara</trd>,
    <trd>k&lcaron;ukat&aacute;</trd>
  </trdgrp>
  <trd lng="sv">sicksacklinje</trd>
  <trd lng="uk">&c_z;&c_i;&c_gu;&c_z;&c_a;&c_gu;</trd>
  <trdgrp lng="zh">
    <trd>&#x4E4B;&#x5B57;&#x5F62; <pr>zh&imacron;z&igrave;x&iacute;ng</pr></trd>,
    <trd>&#x4E4B;&#x5B57;&#x8DEF; <pr>zh&imacron;z&igrave;l&ugrave;</pr></trd>,
    <trd>&#x92F8;&#x9F52;&#x5F62; <pr>j&ugrave;ch&#x1D0;x&iacute;ng</pr></trd>,
    <trd>&#x952F;&#x9F7F;&#x5F62; <pr>j&ugrave;ch&#x1D0;x&iacute;ng</pr></trd>,
    <trd>&#x873F;&#x8712;&#x66F2;&#x6298; <pr>w&amacron;ny&aacute;nq&umacron;zh&eacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="zigzag.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="zigzag.0i.FORM">
    <dif>
      <tld lit="Z"/>e iri:
      <ekz>
        sabla vojeto <tld/>as inter arbustoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la trikiloj sen&ccirc;ese <tld/>is inter &scirc;iaj <klr>[&#x2026;]</klr> fingroj
        <fnt>
          <bib>HsT</bib>
          <lok>Heredanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        akraj komandovortoj <tld/>is en la aero
        <fnt>
          <bib>LGA</bib>
          <lok>II.</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="serpen.0umi.forme">serpentumi</ref>,
      <ref cel="sinu.0i.FORM">sinui</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_u;&c_h;&c_a;&c_c;&c_c;&c_a;
  &c_z;&c_ib;&c_g;&c_z;&c_a;&c_g;&c_a;&c_m;</trd>
  <trd lng="ca">zigzaguejar</trd>
  <trdgrp lng="cs">
    <trd>j&iacute;t klikat&ecaron;</trd>,
    <trd>klikatit se</trd>,
    <trd>kli&ccaron;kovat</trd>,
    <trd>pot&aacute;cet se</trd>,
    <trd>vr&aacute;vorat</trd>,
    <trd>&scaron;n&ecaron;rovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><klr>im </klr>Zickzack <ind>laufen</ind></trd>,
    <trd>herumzackern</trd>
  </trdgrp>
  <trd lng="el">&kappa;&alfa_ton;&ny;&omega; &zeta;&jota;&gamma;&kappa;-&zeta;&alfa;&gamma;&kappa;</trd>
  <trd lng="en">zigzag</trd>
  <trd lng="es">zigzaguear</trd>
  <trd lng="fr">zigzaguer</trd>
  <trd lng="hu">cik&aacute;zik</trd>
  <trd lng="it">zigzagare</trd>
  <trd lng="ja">&#x30B8;&#x30B0;&#x30B6;&#x30B0;&#x306B;&#x9032;&#x3080;  <pr>&#x3058;&#x3050;&#x3056;&#x3050;&#x306B;&#x3059;&#x3059;&#x3080;</pr></trd>
  <trd lng="nl">zigzag lopen</trd>
  <trdgrp lng="pl">
    <trd>zygzakowa&cacute;</trd>,
    <trd>wi&cacute; si&eogonek;</trd>
  </trdgrp>
  <trd lng="pt">ziguezaguear</trd>
  <trdgrp lng="sk">
    <trd>k&lcaron;u&ccaron;kova&tcaron;</trd>,
    <trd>pot&aacute;ca&tcaron; sa</trd>,
    <trd>&iacute;s&tcaron; k&lcaron;ukato</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_b;&c_i;&c_t;&c_i; &c_z;&c_i;&c_gu;&c_z;&c_a;&c_gu;&c_i;</trd>,
    <trd>&c_ib;&c_t;&c_i; &c_z;&c_i;&c_gu;&c_z;&c_a;&c_gu;&c_a;&c_m;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_p;&c_i;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i; &rightquot;&c_m;&c_i;&c_s;&c_l;&c_ib;&c_t;&c_je;&#x201D;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x86C7;&#x884C; <pr>sh&eacute;x&iacute;ng</pr></trd>,
    <trd>&#x5F2F;&#x66F2; <pr>w&amacron;nq&umacron;</pr></trd>,
    <trd>&#x5F4E;&#x66F2; <pr>w&amacron;nq&umacron;</pr></trd>
  </trdgrp>
</drv>
</art>
