<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kvit.xml,v 1.19 2023/09/01 15:46:47 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kvit</rad>/a
</kap>
<drv mrk="kvit.0a">
  <kap>
    <ofc>*</ofc><tld/>a
  </kap>
  <snc mrk="kvit.0a.sxuldo">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Plene liberigita de &scirc;uldo:
      <ekz>
        post tiu &ccirc;i pago mi estos <tld/>a al vi;
      </ekz>
      <ekz>
        li ne estos <tld/>a per dek mil dolaroj;
      </ekz>
      <ekz>
        bieno <tld/>a de &ccirc;iu hipoteko.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>schuldenfrei</trd>,
      <trd>quitt</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>ad&oacute;ss&aacute;gmentes</trd>,
      <trd>kvitt</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>schuldenvrij</trd>,
      <trd>schuldeloos</trd>,
      <trd>schuldloos</trd>
    </trdgrp>
  </snc>
  <snc mrk="kvit.0a.devo">
    <dif>
      Plene liberigita de devo:
      <ekz>
        li kredas per tio, esti tute <tld/>a pri la devoj de amikeco.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>im <ind>Reinen</ind></trd>,
      <trd>frei (von Verpflichtungen)</trd>
    </trdgrp>
    <trd lng="hu">mentes (k&ouml;telezetts&eacute;g al&oacute;l)</trd>
    <trd lng="nl">verplichtingsloos</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_v;&c_ib;&c_t;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y; <klr>(&c_a;&c_d; &c_d;&c_a;&c_w;&c_g;&c_o;&c_w;, &c_a;&c_b;&c_a;&c_v;&c_ja;&c_z;&c_a;&c_c;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_a;&c_w;)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>square</trd>,
    <trd>even</trd>,
    <trd>paid up</trd>,
    <trd>debt-free</trd>,
    <trd>free of debt</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>quitte</trd>,
    <trd>lib&eacute;r&eacute; (d'une dette, d'une obligation)</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>in <ind>pari</ind></trd>,
    <trd>libero<klr> (da debito o dovere)</klr></trd>,
    <trd>assolto<klr> (da debito od obbligo)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x514D;&#x308C;&#x305F;  [<ind>&#x307E;&#x306C;&#x304C;&#x308C;&#x305F;</ind>]</trd>,
    <trd>&#x501F;&#x91D1;&#x306E;&#x306A;&#x304F;&#x306A;&#x3063;&#x305F;  [<ind>&#x3057;&#x3083;&#x3063;&#x304D;&#x3093;&#x306E;&#x306A;&#x304F;&#x306A;&#x3063;&#x305F;</ind>]</trd>,
    <trd>&#x514D;&#x9664;&#x3055;&#x308C;&#x305F;  [<ind>&#x3081;&#x3093;&#x3058;&#x3087;&#x3055;&#x308C;&#x305F;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>quitte</trd>,
    <trd>kiet</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>skwitowany</trd>,
    <trd>wolny od d&lstroke;ug&oacute;w</trd>
  </trdgrp> 
  <trdgrp lng="pt">
    <trd>quite</trd>,
    <trd>desobrigado</trd>
  </trdgrp>
  <trd lng="ro">liber de datorii</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_i;&c_ch;&c_je;&c_g;&c_o; &c_n;&c_je; &c_d;&c_o;&c_l;&c_zh;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_v;&c_i;&c_t;&c_a;&c_v;&c_sh;&c_i;&c_j;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_ib;&c_ch;&#x43E;&c_g;&#x43E; &#x43D;&c_je; &c_v;&c_i;&#x43D;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_ib;&#x43B;&#x44C;&#x43D;&c_i;&c_j; &c_v;&c_ib;&c_d; &c_b;&#x43E;&c_r;&c_g;&c_u;</trd>
  </trdgrp>
</drv>

<drv mrk="kvit.0igi">
  <kap>
    <tld/>igi
  </kap>
  <snc>
    <dif>
      Fari iun <tld/>a:
      <ekz>
        mi <tld/>igas vin de la resto de via &scirc;uldo, de via vizito;
      </ekz>
      <ekz>
        mi <tld/>i&gcirc;os al vi la venontan monaton;
      </ekz>
      <ekz><tld/>i&gcirc;as servo per reservo<fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_v;&c_ib;&c_t;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="en">
    <trd>clear</trd>,
    <trd>release from debt</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>dispenser (d'une obligation)</trd>,
    <trd>exon&eacute;rer</trd>,
    <trd>tenir <ind>quitte</ind></trd>
  </trdgrp>
  <trd lng="hu">mentes&iacute;t (ad&oacute;ss&aacute;g, k&ouml;telezetts&eacute;g al&oacute;l)</trd>
  <trdgrp lng="it">
    <trd>assolvere<klr> (da debito od obbligo)</klr></trd>,
    <trd>liberare<klr> (da debito od obbligo)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x501F;&#x3092;&#x8FD4;&#x3059;  [<ind>&#x304B;&#x308A;&#x3092;&#x304B;&#x3048;&#x3059;</ind>]</trd>,
    <trd>&#x652F;&#x6255;&#x3046;  [<ind>&#x3057;&#x306F;&#x3089;&#x3046;</ind>]</trd>,
    <trd>&#x514D;&#x9664;&#x3059;&#x308B;  [<ind>&#x3081;&#x3093;&#x3058;&#x3087;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kwijtschelden</trd>,
    <trd>vereffenen</trd>,
    <trd>verrekenen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>odd&lstroke;u&zdot;y&cacute;</trd>,
    <trd>sp&lstroke;aci&cacute;</trd>
  </trdgrp>   
  <trd lng="ro">ipotecare</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&#x43E;&c_b;&c_i;&c_t;&c_i; &#x43A;&c_v;&c_i;&c_t;&c_a;&#x43C;&c_i;</trd>,
    <trd>&c_r;&#x43E;&c_b;&c_i;&c_t;&c_i; &c_v;&c_ib;&#x43B;&#x44C;&#x43D;&c_i;&#x43C; &c_v;&c_ib;&c_d; &c_b;&#x43E;&c_r;&c_g;&c_ib;&c_v;</trd>
  </trdgrp>
</drv>
</art>
