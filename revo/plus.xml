<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>plus</rad>
</kap>
<drv mrk="plus.0">
  <kap><tld/></kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PV</bib></fnt>
  <snc mrk="plus.0.MAT">
    <dif>
      Konjunkcio indikanta <ref cel="adici.0o">adicion</ref> a&ubreve;
      pozitivan <ref tip="super" cel="sign.0o.MAT">signon</ref>:
      <ekz>
       <frm>4+5 = 9</frm>
        <klr>(legu: kvar <tld/> kvin estas na&ubreve;)</klr>;
      </ekz>
      <ekz>
        temperaturo je <frm>+25&ring;C</frm>
        <klr>(legu: <tld/> dudek kvin gradoj celsiaj)</klr>.
      </ekz>
    </dif>
    <ref tip="ant" cel="minus.0">minus;</ref>
    <ref tip="vid" cel="kaj.0">kaj.</ref>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ju;&c_s;</trd>
  <trd lng="de">plus</trd>
  <trd lng="en">plus</trd>
  <trd lng="fr">plus</trd>
  <trdgrp lng="hu">
    <trd>plusz</trd>,
    <trd>meg</trd>
  </trdgrp>
  <trd lng="nl">plus</trd>
  <trd lng="pl">plus</trd>
  <trd lng="ru">&c_p;&c_l;&c_ju;&c_s;</trd>
</drv>

<drv mrk="plus.0o">
  <kap><tld/>o</kap>
  <snc mrk="plus.0o.signo">
    <uzo tip="fak">KOMP</uzo>
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>KompLeks</bib></fnt>
    <uzo tip="fak">TIP</uzo>
    <dif>
      La krucforma <ref tip="super" cel="sign.0o.KOMP">signo</ref>
      <frm>+</frm>, uzata kiel simbolo de <ref cel="plus.0"><tld/></ref>,
      kaj por diversaj aliaj celoj:
      <ekz>
        la &scirc;akistoj uzas <tld/>on por indiki &scirc;akon.
      </ekz>
    </dif>
    <ref tip="vid" cel="minus.0o">minuso</ref>
    <rim>
      Troveblas <ctl><tld/>signo</ctl> en <fnt><bib>MatVort</bib></fnt>.
    </rim>
    <trd lng="be">&c_p;&c_l;&c_ju;&c_s; <klr>(&c_z;&c_n;&c_a;&c_k;)</klr></trd>
    <trd lng="cs">plus</trd>
    <trdgrp lng="de">
       <trd>Additionszeichen</trd>,
       <trd>Pluszeichen</trd>
    </trdgrp>
    <trd lng="en">plus sign</trd>
    <trd lng="es">m&aacute;s</trd>
    <trd lng="fr">signe plus</trd>
  <trdgrp lng="hu">
    <trd>&ouml;sszead&aacute;sjel</trd>,
    <trd>pluszjel</trd>
  </trdgrp>
    <trd lng="nl">pluspunt</trd>
    <trd lng="pl">plus</trd>
    <trd lng="ru">&c_z;&c_n;&c_a;&c_k; <ind>&c_p;&c_l;&c_ju;&c_s;</ind></trd>
    <trd lng="sv">plus</trd>
  </snc>
  <snc mrk="plus.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Pozitiva nombro, avanta&gcirc;o, malmanko:
      <ekz>
        naski&gcirc;a <tld/>o <klr>(la nombro, per kiu la naski&gcirc;oj
        superas la mortojn)</klr>
        <fnt><bib>PIV1</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_l;&c_ju;&c_s;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_a;&c_v;&c_a;&c_g;&c_a;</trd>
    </trdgrp>
    <trd lng="de">Plus</trd>
    <trdgrp lng="de">
       <trd>Mehr</trd>,
       <trd>Plus</trd>,
       <trd>&Uuml;berschuss</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>exc&eacute;dent</trd>,
      <trd>gain</trd>,
      <trd>plus<klr tip="ind"> (gain)</klr></trd>,
      <trd>surplus</trd></trdgrp>
    <trdgrp lng="hu">
      <trd>plusz <klr tip="ind">(t&ouml;bblet)</klr></trd>,
      <trd>t&ouml;bblet</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_ju;&c_s;</trd>,
      <trd>&c_d;&c_o;&c_b;&c_a;&c_v;&c_k;&c_a;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_b;&c_y;&c_l;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_r;&c_o;&c_s;&c_t;</trd>,
      <trd>&c_p;&c_r;&c_je;&c_v;&c_y;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
</drv>
</art>
