<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: plus.xml,v 1.40 2024/02/17 07:52:16 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>plus</rad> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="plus.0">
  <kap><tld/></kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="plus.0.MAT">
    <dif>
      Konjunkcio indikanta <ref cel="adici.0o">adicion</ref> a&ubreve;
      pozitivan <ref tip="super" cel="sign.0o.MAT">signon</ref>:
      <ekz>
       <frm>4+5 = 9</frm>
        <klr>(legu: kvar <tld/> kvin estas na&ubreve;)</klr>;
      </ekz>
      <ekz>
        temperaturo je <frm>+25&#x2DA;C</frm>
        <klr>(legu: <tld/> dudek kvin gradoj celsiaj)</klr>.
      </ekz>
    </dif>
    <ref tip="ant" cel="minus.0">minus;</ref>
    <ref tip="vid" cel="kaj.0">kaj.</ref>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ju;&c_s;</trd>
  <trd lng="ca">m&eacute;s</trd>
  <trd lng="cs">plus</trd>
  <trd lng="de">plus</trd>
  <trd lng="en">plus</trd>
  <trd lng="es">m&aacute;s <klr>(conj.)</klr></trd>
  <trd lng="fr">plus</trd>
  <trdgrp lng="hu">
    <trd>plusz</trd>,
    <trd>meg</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30D7;&#x30E9;&#x30B9;  <pr>&#x3077;&#x3089;&#x3059;</pr></trd>,
    <trd>&#xFF5E;<ind>&#x3092;&#x8DB3;&#x3057;&#x3066;  </ind><pr>&#x3092;&#x305F;&#x3057;&#x3066;</pr></trd>
  </trdgrp>
  <trd lng="nl">plus</trd>
  <trd lng="pl">plus</trd>
  <trd lng="pt">mais <klr>(conj.)</klr></trd>
  <trd lng="ru">&c_p;&c_l;&c_ju;&c_s;</trd>
  <trd lng="sk">plus</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43B;&#x44E;&c_s;</trd>,
    <trd>&c_v;&c_i;&c_shch;&c_je; &#x43D;&c_u;&#x43B;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x52A0; <pr>ji&#x101;</pr></trd>,
    <trd>&#x52A0;&#x4E0A; <pr>ji&#x101;sh&#xE0;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="plus.0o">
  <kap><tld/>o</kap>
  <snc mrk="plus.0o.signo">
    <uzo tip="fak">KOMP</uzo>
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>KompLeks</bib></fnt>
    <uzo tip="fak">TIP</uzo>
    <dif>
      La krucforma <ref tip="super" cel="sign.0o.KOMP">signo</ref>
      <frm>+</frm>, uzata kiel simbolo de <ref cel="plus.0"><tld/></ref>,
      kaj por diversaj aliaj celoj:
      <ekz>
        la &scirc;akistoj uzas <tld/>on por indiki &scirc;akon.
      </ekz>
    </dif>
    <ref tip="vid" cel="minus.0o">minuso</ref>
    <rim>
      Troveblas <ctl><tld/>signo</ctl> en <fnt><bib>MatVort</bib></fnt>.
    </rim>
    <trd lng="be">&c_p;&c_l;&c_ju;&c_s; <klr>(&c_z;&c_n;&c_a;&c_k;)</klr></trd>
    <trd lng="cs">plus</trd>
    <trdgrp lng="de">
       <trd>Additionszeichen</trd>,
       <trd>Pluszeichen</trd>
    </trdgrp>
    <trd lng="en">plus sign</trd>
    <trdgrp lng="es">
      <trd>signo m&aacute;s <klr tip="amb">(+)</klr></trd>,
      <trd>signo de <ind>adici&oacute;n</ind></trd>
    </trdgrp>
    <trd lng="fr">signe plus</trd>
    <trdgrp lng="hu">
      <trd>&ouml;sszead&aacute;sjel</trd>,
      <trd>pluszjel</trd>
    </trdgrp>
    <trd lng="nl">pluspunt</trd>
    <trd lng="pl">znak <ind>plus</ind></trd>
    <trdgrp lng="pt">
      <trd>mais <klr tip="amb">(+)</klr></trd>,
      <trd>sinal de <ind>adi&ccedil;&atilde;o</ind></trd>
    </trdgrp>
    <trd lng="ru">&c_z;&c_n;&c_a;&c_k; <ind>&c_p;&c_l;&c_ju;&c_s;</ind></trd>
    <trd lng="sv">plus</trd>
    <trdgrp lng="zh">
    <trd>&#x6B63;&#x53F7; <pr>zh&#xE8;ngh&#xE0;o</pr></trd>,
    <trd>&#x6B63;&#x865F; <pr>zh&#xE8;ngh&#xE0;o</pr></trd>,
    <trd>&#x52A0;&#x53F7; <pr>ji&#x101;h&#xE0;o</pr></trd>,
    <trd>&#x52A0;&#x865F; <pr>ji&#x101;h&#xE0;o</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="plus.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Pozitiva nombro, avanta&gcirc;o, malmanko:
      <ekz>
        naski&gcirc;a <tld/>o <klr>(la nombro, per kiu la naski&gcirc;oj
        superas la mortojn)</klr>
        <fnt><bib>PIV1</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_l;&c_ju;&c_s;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_a;&c_v;&c_a;&c_g;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
       <trd>Mehr</trd>,
       <trd>Plus</trd>,
       <trd>&Uuml;berschuss</trd>
    </trdgrp>
    <trd lng="en">surplus</trd>
    <trdgrp lng="es">
      <trd>excedente</trd>,
      <trd>ganancia</trd>,
      <trd>plus</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>exc&eacute;dent</trd>,
      <trd>gain</trd>,
      <trd>plus<klr tip="ind"> (gain)</klr></trd>,
      <trd>surplus</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>plusz <klr tip="ind">(t&ouml;bblet)</klr></trd>,
      <trd>t&ouml;bblet</trd>
    </trdgrp>
    <trd lng="nl">overschot</trd>
    <trd lng="pl">zaleta</trd>
    <trd lng="pt">excedente</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_ju;&c_s;</trd>,
      <trd>&c_d;&c_o;&c_b;&c_a;&c_v;&c_k;&c_a;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_b;&c_y;&c_l;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_r;&c_o;&c_s;&c_t;</trd>,
      <trd>&c_p;&c_r;&c_je;&c_v;&c_y;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x8FC7;&#x5269; <pr>gu&#xF2;sh&#xE8;ng</pr></trd>,
    <trd>&#x904E;&#x5269; <pr>gu&#xF2;sh&#xE8;ng</pr></trd>,
    <trd>&#x4EC2; <pr>l&#xE8;</pr></trd>,
    <trd>&#x4F59;&#x5269; <pr>y&#xFA;sh&#xE8;ng</pr></trd>,
    <trd>&#x9918;&#x5269; <pr>y&#xFA;sh&#xE8;ng</pr></trd>,
    <trd>&#x4F59;&#x88D5; <pr>y&#xFA;y&#xF9;</pr></trd>,
    <trd>&#x9918;&#x88D5; <pr>y&#xFA;y&#xF9;</pr></trd>,
    <trd>&#x76C8;&#x5229; <pr>y&#xED;ngl&#xEC;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="ca">
    <trd>m&eacute;s</trd>,
    <trd>plus</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>p&rcaron;ebytek</trd>,
    <trd>p&rcaron;&iacute;r&uring;stek</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8D85;&#x904E;  <pr>&#x3061;&#x3087;&#x3046;&#x304B;</pr></trd>,
    <trd>&#x4F59;&#x5270;  <pr>&#x3088;&#x3058;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="pl">plus</trd>
  <trd lng="sk">pr&iacute;rastok</trd>
  <trd lng="uk">&#x43F;&#x43E;&c_z;&c_i;&c_t;&c_i;&c_v;&#x43D;&c_je; &c_ch;&c_i;&c_s;&#x43B;&#x43E;</trd>
</drv>
</art>
