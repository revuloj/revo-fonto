<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rond.xml,v 1.34 2020/02/16 00:10:52 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>rond</rad>/o
</kap>
<drv mrk="rond.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <fnt><bib>VdE</bib></fnt>
  <snc mrk="rond.0o.FORM">
    <dif>
      Io pli-malpli sam<ref tip="lst" lst="voko:formoj"
      cel="form.0o.aspekto">forma<sncref/></ref> 
      kiel <ref cel="cirkl.0o">cirklo</ref>:
      <ekz>
        danci en <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 21:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        niaj danc<tld/>oj aliformi&gcirc;is en funebron  
        <fnt>
          <bib>MT</bib>
          <lok>Plorkanto 5:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        la aran&gcirc;o de radoj de veturilo, iliaj aksoj kaj aksingoj kaj
        radradioj kaj rad<tld/>oj, &ccirc;io estis fandita
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 7:33</lok>
        </fnt>;
      </ekz>
      <ekz>
        super <klr>[la mar&ccirc;o]</klr> ili &scirc;vebis en grandaj 
        <tld/>oj
        <fnt><bib>Fab4</bib>, <lok>Filino de la mar&ccirc;a re&gcirc;o
        </lok></fnt>;
      </ekz>
      <ekz>
        &gcirc;i turni&gcirc;adis en <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Persista stana soldato</lok>
        </fnt>;
      </ekz>
      <ekz>
        la rabistoj sidis en <tld/>o &ccirc;irka&ubreve; la fajro
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
         la senplumaj birdidoj stari&gcirc;as en <tld/>o &ccirc;irka&ubreve; la <tld/>o de
         la nesto
        <fnt>
          <bib>Fab3</bib>
          <lok>Cigna nesto</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la herbo malanta&ubreve; la dikaj trunkoj brilis la oraj strioj de 
        la sunaj radioj, sur la tero, en la aleo tremis oraj <tld/>oj
        kaj <tld/>etoj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        magia <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        karusel' <klr>[&#8230;]</klr> centrifuga <tld/>o
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="http://steloj.de/esperanto/butiko">La Stranga
          Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        miaj &gcirc;isnunaj provoj ne ampleksis ja ankora&ubreve; la tutan 
        <tld/>on <klr>(<ref
        cel="kamp.0o.tereno">kampon<sncref/></ref>)</klr> 
        da diversaj homaj laboroj
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="ir.rond0i">rondiri.</ref>
    <trd lng="de">Rondell</trd>
    <trd lng="fr">rond<klr tip="ind"> (subst.)</klr></trd>
    <trdgrp lng="pl">
    <trd>ko&lstroke;o</trd>,
    <trd>kr&aogonek;g</trd>
    </trdgrp>
  </snc>
  <snc mrk="rond.0o.ciklo">
    <dif>
      <ref tip="dif" cel="cikl.0o.peri">Ciklo<sncref/></ref>:
      <ekz>
        la <tld/>o de la festenaj tagoj estis finita
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 1:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        formi&gcirc;as <tld/>o da eraroj, kiu kondukas al &ccirc;iam plua mallevi&gcirc;o 
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la sen&ccirc;esa <tld/>o de nasko kaj morto
        <fnt>
          <bib>Monato</bib>
          <aut>Guido Van Damme</aut>
          <vrk><url
          ref="http://steloj.de/esperanto/monato2/006958.html">Psiko sen
          karno</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &gcirc;i ne apartenis al la <tld/>o de liaj pensoj
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Zyklus</trd>,
      <trd>Reihe</trd>
    </trdgrp>
  </snc>
  <snc mrk="rond.0o.vojo">
    <dif>
      Revena vojo, la&ubreve; kiu iu iras, aparte gardistoj;
      <ref tip="sin" cel="rund.0o">rundo</ref>:
      <ekz>
        policistoj <tld/>iris sur la stratoj kriegante dulingve
        &leftquot;Dan&gcirc;ero! Febrorisko!&#8221;
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2006/008875.php">Mallonga historio de
          la evonima milito</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>voja&gcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2015/011241.php">Rondvoja&gcirc;i
          trajne</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Runde</trd>
  </snc>
  <snc mrk="rond.0o.cirklo">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="dif" cel="cirkl.0o.linio">Cirklo (linio)</ref> a&ubreve;
      <ref tip="dif" cel="cirkl.0o.surfaco">cirklo (surfaco)</ref>.
    </dif>
  </snc>
  <snc mrk="rond.0o.societo">
    <dif>
      Aparta aro da homoj,
      <ref tip="vid" cel="medi.0o.POL">medio</ref>,
      kunvenantaro,
      <ref tip="vid" cel="societ.0o.institucio">societo</ref>:
      <ekz>
        li multe klopodis por favorigi al Esperanto la oficialajn kaj
        sciencajn <tld/>ojn;
      </ekz>
      <ekz>
        kura&gcirc;e, ho nia <tld/>o
        <fnt>
          <aut>L. L. Zamenhof</aut>
          <vrk>Al la Fratoj</vrk>
        </fnt>!
      </ekz>
      <ekz>
        tio havas intereson nur por limigita <tld/>o el niaj legantoj;
      </ekz>
      <ekz>
        en <tld/>o de piuloj kaj en kunveno 
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 111:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi havos &ccirc;irka&ubreve; mi familian <tld/>on
        <fnt>
          <bib>Fab2</bib>
          <lok>Najbaraj familioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        kion havas por fari la pasero en la <tld/>o de la gruoj
        <klr>(inter la gruoj)</klr>
        <fnt><bib>Fab4</bib>, <lok>La vento rakontas pri Valdemaro 
        Doe...</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i aperis en la <tld/>o de viroj
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kantista <tld/>o de Wartburg
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>
      <ekz>
        drinko<tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
        </fnt>;
      </ekz>
      <ekz>
        Hus akiris subtenon de la tiamaj influaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url
          ref="http://steloj.de/esperanto/monato2/007031.html">Katolikismo:
           Johano Hus &#8208; &ccirc;u ne plu herezulo?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Runde</trd>
    <trd lng="fr">cercle<klr tip="ind"> (de personne)</klr></trd>
    <trd lng="nl">gezelschap</trd>
    <trdgrp lng="pl">
      <trd>ko&lstroke;o</trd>,
      <trd>k&oacute;&lstroke;ko</trd>,
      <trd>kr&aogonek;g</trd>
    </trdgrp>
  </snc>
  <snc mrk="rond.0o.rundo">
    <dif>
      <ref tip="dif" cel="rund1.0o.POL">Rundo<sncref/></ref>:
      <ekz>
        la SMS-tekstomesa&gcirc;oj, <klr>[&#8230;]</klr> la&ubreve; pluraj analizoj, grave
        influis elektontojn inter la du vo&ccirc;donaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Istv&aacute;n Ertl</aut>
          <vrk><url ref="https://www.monato.be/2003/007869.html">Vo&ccirc;o de
          vo&ccirc;donantoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; la unua balot<tld/>o okazis debatoj dissenditaj en radio-
        kaj televidaj kanaloj
        <fnt>
          <bib>Monato</bib>
          <aut>Juh&aacute;sz K&aacute;roly</aut>
          <vrk><url ref="&Monato;/2006/008964.php">Konvinka
          venko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
     <ref cel="rauxnd.0o">ra&ubreve;ndo</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_u;&c_g;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kolo</trd>,
    <trd>krou&zcaron;ek</trd>,
    <trd>kruh</trd>,
    <trd>kruhov&yacute; tvar</trd>,
    <trd>okruh</trd>,
    <trd>sdru&zcaron;en&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kreis</trd>,
    <trd>Rund</trd>
  </trdgrp>
  <trd lng="en">circle</trd>
  <trd lng="hu">k&ouml;r</trd>
  <trd lng="nl">kring</trd>
  <trd lng="ru">&c_k;&c_r;&c_u;&c_g;</trd>
  <trdgrp lng="sk">
    <trd>kruh</trd>,
    <trd>kruhov&yacute; tvar</trd>,
    <trd>okruh</trd>,
    <trd>zdru&zcaron;enie</trd>
  </trdgrp>
</drv>

<drv mrk="rond.0a">
  <kap><tld/>a</kap>
  <snc mrk="rond.0a.FORM">
    <dif>
      Havanta formon, kiu similas al tiu de <ref
      cel="cirkl.0o">cirklo</ref>
      a&ubreve; <ref cel="sfer.0o">sfero</ref>:
      <ekz>
        <tld/>a turo, fenestro, tablo, bu&scirc;eto;
      </ekz>
      <ekz>
        <tld/>a pilko, kapo, kuglo;
      </ekz>
      <ekz>
        via umbiliko estas kiel <tld/>a pokalo
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 7:2</lok>
        </fnt>;
      </ekz>
      <ekz>
         la re&gcirc;o Arturo kaj <klr>[&#8230;]</klr> la herooj de lia <tld/>a
         tablo
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        abundaj bukloj ondi&gcirc;adis &ccirc;irka&ubreve; la <tld/>aj &scirc;ultroj
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
         bela knabino kun blondaj bukloj, kun ru&gcirc;aj <tld/>aj vangoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Avineto</lok>
        </fnt>;
      </ekz>
      <ekz>
         stranga <tld/>a objekto, <klr>[&#8230;]</klr> tio estis malnova pilko
        <fnt>
          <bib>Fab2</bib>
          <lok>Gefian&ccirc;oj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;arma milda viza&gcirc;o, kiu estis <tld/>a kaj floris kiel rozo
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis vigla kaj bone nutrita, grasa kaj <tld/>a
        <fnt>
          <bib>Fab3</bib>
          <lok>Bona humoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj kiel ovoj de birdoj
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="cs">kulat&yacute;</trd>
    <trdgrp lng="hu">
      <trd>kerek</trd>,
      <trd>k&ouml;r alak&uacute;</trd>
    </trdgrp>
    <trd lng="pl">okr&aogonek;g&lstroke;y</trd>
    <trd lng="ru">&c_k;&c_r;&c_u;&c_g;&c_l;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="rond.0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <dif>
      Prezentanta kvanton rigardatan kiel simplan, senmankan:
      <ekz>
        Jes! Jes! &mdash; kriadis la grupanoj el &ccirc;iu direkto &ndash; ni estu 
        tridekope, tridek estas <tld/>a nombro
        <fnt>
          <aut>Hevesi L., trad. Vizi L.</aut>,
          <vrk><url
          ref="https://sites.google.com/site/laaventurojdeandreojelky">
          La aventuroj de Andreo Jelky</url></vrk>,
          <lok>[2010]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>kruhov&yacute;</trd>,
    <trd>okrouhl&yacute;</trd>
  </trdgrp>
  <trd lng="de">rund</trd>
  <trd lng="fr">rond<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="pl">okr&aogonek;g&lstroke;y</trd>
  <trdgrp lng="sk">
    <trd>kruhovit&yacute;</trd>,
    <trd>okr&uacute;hly</trd>
  </trdgrp>
</drv>

<drv mrk="rond.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rond.0igi.gxenerale">
    <dif>
      Igi <tld/>a:
      <ekz>
        la ondoj de la maro iom post iom <tld/>igas la angulecan pecon
        da roko
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#8230;]</klr> metis ilin en la tornilon kaj <tld/>igis
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fabelo pri iu, kiu migris por ekkoni timon</lok>
        </fnt>;
      </ekz>
      <ekz>
         <tld/>igi sian dorson anta&ubreve; altestimataj sinjoroj
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2005/008555.php">&Scirc;uisto kaj
          silkokomercisto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>igi la tuton <klr>(<ref
        cel="perfek.0igi">perfektigi</ref>)</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk><url
          ref="http://steloj.de/esperanto/monato2/006813.html">Sa&gcirc;a kapo
          duonvorton komprenas</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">lekerek&iacute;t</trd>    
  </snc>
  <snc mrk="rond.0igi.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <dif>
      <klr>(<ref cel="nombr.0o.MAT">nombron</ref>)</klr>
      <ref cel="aproks.0i">Aproksimi</ref> &gcirc;in per alia nombro kun
      malpli da <ref cel="decima.0o">decimaloj</ref> a&ubreve;, por entjero,
      malpli da nenulaj ciferoj en la dekstra parto:
      <ekz>
        <tld/>igante <frm am="pi">&pi;</frm> je <frm>2</frm> decimaloj, 
        oni ricevas <frm>3,14</frm>;
      </ekz>
      <ekz>
        <tld/>igita al 24 bitoj en duuma unuobla precizeca glitkoma
        aran&gcirc;o
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref="https://eo.wikipedia.org/?curid=319482">Glitkomo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igu vian pagon &gcirc;is la supera deko <klr>(ekz-e: pagu
        <frm>40</frm> anstata&ubreve; <frm>32</frm>)</klr>;
      </ekz>
      <ekz>
        la <ind><tld/>igo</ind> enkondukis tro altan eraron.
        <trd lng="be">&c_a;&c_k;&c_r;&c_u;&c_g;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
        <trd lng="de">Rundung</trd>
        <trd lng="en">rounding</trd>
        <trd lng="fr">arrondi</trd>
        <trd lng="hu">kerek&iacute;t&eacute;s</trd>
        <trd lng="pl">zaokr&aogonek;glanie</trd>
        <trd lng="ru">&c_o;&c_k;&c_r;&c_u;&c_g;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
      </ekz>
    </dif>                                
    <trd lng="hu">kerek&iacute;t</trd>    
  </snc>
  <trd lng="be">&c_a;&c_k;&c_r;&c_u;&c_g;&c_l;&c_ja;&c_c;&c_mol;</trd>
  <trd lng="cs">zaokrouhlovat</trd>
  <trd lng="de">runden</trd>
  <trd lng="en">round</trd>
  <trd lng="fr">arrondir</trd>
  <trd lng="pl">zaokr&aogonek;gli&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_k;&c_r;&c_u;&c_g;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_k;&c_r;&c_u;&c_g;&c_l;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zagu&lcaron;acova&tcaron;</trd>
</drv>

<drv mrk="rond.trafik0o">
  <kap>trafik<tld/>o</kap>
  <snc mrk="rond.trafik0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      Loko, kie vojoj renkonti&gcirc;as,
      kaj veturiloj ronde &ccirc;irka&ubreve;iras centran pecon
      por transiri al alia vojo:
    </dif>
    <refgrp tip="sin">
      <ref cel="cirkl.trafik0o.TRA">trafikcirklo</ref>;
    </refgrp>
    <refgrp tip="vid">
      <ref cel="kruc.voj0igxo.TRA">vojkruci&gcirc;o</ref>,
      <ref cel="nod.voj0o.TRA">vojnodo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_l;&c_mol;&c_c;&c_a;&c_v;&c_a;&c_je; <ind>&c_s;&c_k;&c_r;&c_y;&c_zh;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</ind></trd>
  <trd lng="da">rundk&oslash;rsel</trd>
  <trd lng="de">Kreisverkehr</trd>
  <trdgrp lng="en">
    <trd>roundabout</trd>,
    <trd>traffic circle</trd>
  </trdgrp>
  <trdgrp lng="es">
   <trd>glorieta</trd>,
   <trd>rotonda</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>rond-point</trd>,
    <trd>giratoire</trd>,
    <trd>carrefour giratoire</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&ouml;rforgalom</trd>,
    <trd>k&ouml;rforgalmi csom&oacute;pont</trd>
  </trdgrp>
  <trdgrp lng="it">
   <trd>isola <ind>rotazionale</ind></trd>, 
   <trd>rotatoria</trd> 
  </trdgrp>
