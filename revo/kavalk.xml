<art mrk="$Id: kavalk.xml,v 1.16 2022/03/21 17:51:57 revo Exp $">
<kap>
  <rad>kavalkad</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kavalk.0o">
  <kap><tld/>o</kap>
  <snc mrk="kavalk.0o.RAJ">
    <dif>
      Pompa ceremonia a&ubreve; plezura promenado de rajdantaro:
      <ekz>
        montri&gcirc;is survoje brua kaj brila <tld/>o
        <fnt><bib>Mary</bib></fnt>.
      </ekz>  
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_k;&c_a;&c_d;&c_a;</trd>
  <trdgrp lng="bg">
    <trd>&c_k;&c_a;&c_l;&c_v;&c_a;&c_k;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_sh;&c_je;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>jezdeck&aacute; promen&aacute;da</trd>,
    <trd>kavalk&aacute;da</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kavalkade</trd>,
    <trd>Reiteraufzug</trd>
  </trdgrp>
  <trd lng="en">cavalcade</trd>
  <trd lng="fr">cavalcade<klr tip="ind"> (promenade,
  c&eacute;r&eacute;monie)</klr></trd>
  <trdgrp lng="hu">
    <trd>lovasfelvonul&aacute;s</trd>,
    <trd>sz&iacute;nes forgatag</trd>,
    <trd>kavalk&aacute;d</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>cavalcade</trd>,
    <trd>ruiterstoet</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>banderia</trd>,
    <trd>kawalkada</trd>,
    <trd>orszak</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>banderie</trd>,
    <trd>cavalcad&abreve;</trd>,
    <trd>procesiune</trd>
  </trdgrp>  
  <trd lng="ru">&c_k;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_k;&c_a;&c_d;&c_a;</trd>
  <trd lng="sk">jazdeck&aacute; promen&aacute;da</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x430;&#x432;&#x430;&#x43B;&#x44C;&#x43A;&#x430;&#x434;&#x430;</trd>,
    <trd>&#x433;&#x440;&#x443;&#x43F;&#x430; &#x432;&#x435;&#x440;&#x448;&#x43D;&#x438;&#x43A;&#x456;&#x432;</trd>
  </trdgrp>
</drv>
</art>
