  <trd lng="sk">jazdeck&aacute; promen&aacute;da</trd>
  <trdgrp lng="uk">
    <trd>&#1082;&#1072;&#1074;&#1072;&#1083;&#1100;&#1082;&#1072;&#1076;&#1072;</trd>,
    <trd>&#1075;&#1088;&#1091;&#1087;&#1072; &#1074;&#1077;&#1088;&#1096;&#1085;&#1080;&#1082;&#1110;&#1074;</trd>
