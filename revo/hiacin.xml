<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hiacin.xml,v 1.28 2022/04/30 08:42:41 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>hiacint</rad>/o
</kap>
<drv mrk="hiacin.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="liliac.0oj">liliacoj</ref>
      (<trd lng="la">Hyacinthus</trd>),
      bulba vegeta&jcirc;o kun forte odorantaj origine malhelbluaj
      floroj, kun diverskoloraj &gcirc;ardene kulturataj specioj:
      <ekz>
        &ccirc;iuj <tld/>oj kaj tulipoj staris en du longaj vicoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sonoriletoj de la <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        diversaj specioj de plantoj kreskas sur la insulo: ekzemple
        sova&gcirc;a <tld/>o,
        liknido, sileno kaj koleario
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="&Monato;/2004/008289.html">Mistera insulo Ailsa Craig</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/a/ae/Garden_Hyacinth_Hyacinthus_orientalis_'Blue_Jacket'_Flower_2000px.jpg/360px-Garden_Hyacinth_Hyacinthus_orientalis_'Blue_Jacket'_Flower_2000px.jpg" prm="GFDL 1.2">&gcirc;ardena <tld/>o (Hyacinthus orientalis)
        <fnt>
          <aut>(c) 2008 Derek Ramsey (Ram-Man), propra foto</aut>
          <lok><url ref="&WCW;/File:Garden_Hyacinth_Hyacinthus_orientalis_'Blue_Jacket'_Flower_2000px.jpg">&WCW;/File:Garden_Hyacinth_Hyacinthus_orientalis_'Blue_Jacket'_Flower_2000px.jpg</url></lok>
        </fnt>
    </bld>    
    <trd lng="be">&c_g;&c_ib;&c_ja;&c_c;&c_y;&c_n;&c_t;</trd>
    <trd lng="bg">&c_z;&c_ju;&c_m;&c_b;&c_ju;&c_l;</trd>
    <trdgrp lng="br">    
      <trd>louzaouenn-ar-goukoug</trd>,
      <trd>pourenn-vran</trd>
    </trdgrp>
    <trdgrp lng="cs">
      <trd>hyacint</trd>,
      <trd>hyacinta</trd>
    </trdgrp>
    <trd lng="de">Hyazinthe</trd>
    <trd lng="el">&ypsilon;&alfa_ton;&kappa;&jota;&ny;&theta;&omikron;&sigma_fina;</trd>
    <trd lng="en">hyacinth</trd>
    <trd lng="es">jacinto</trd>
    <trd lng="fa">&a_s;&a_n;&a_b;&a_l;</trd>
    <trd lng="fr">jacinthe</trd>
    <trd lng="hu">j&aacute;cint</trd>
    <trd lng="it">giacinto</trd>
    <trd lng="nl">hyacint</trd>
    <trd lng="pl">hiacynt</trd>
    <trd lng="pt">jacinto</trd>
    <trd lng="ru">&c_g;&c_i;&c_a;&c_c;&c_i;&c_n;&c_t;</trd>
    <trd lng="sk">hyacint</trd>
  </snc>
  <rim>
    <ref tip="hom" cel="jacint.0o">Hiacinto a&ubreve; jacinto</ref> estas anka&ubreve; varia&jcirc;o de zirkono.
  </rim>
  <trd lng="uk">&#x433;&#x456;&#x430;&#x446;&#x438;&#x43D;&#x442;</trd>
</drv>
</art>
