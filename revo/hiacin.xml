  <trd lng="nl">hyacint</trd>
  <trd lng="pl">hiacynt</trd>
  <trd lng="pt">jacinto</trd>
  <trd lng="ru">&c_g;&c_i;&c_a;&c_c;&c_i;&c_n;&c_t;</trd>
</drv>
