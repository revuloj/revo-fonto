<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>korp</rad>/o
</kap>
<drv mrk="korp.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="korp.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      &Ccirc;e homoj kaj bestoj, aro de la materiaj partoj, kiuj konsistigas la
      organaron:
      <ekz>
        tio &ccirc;i estas saniga por via <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la <tld/>o estas morta, la animo estas senmorta<fnt>Z</fnt>;
      </ekz>
      <ekz>
        fortika, malsana <tld/>o;
      </ekz>
      <ekz>
        <uzo tip="fak">KAT</uzo>
        la <tld/>o de Jesu-Kristo
        <klr>(e&ubreve;karistio)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        se ili povis al la nomo de ilia projekto ellabori
        anka&ubreve; sinteni povantan <tld/>on<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="karn.0o">karno</ref>,
      <ref cel="kadavr.0o">kadavro</ref>,
      <ref cel="organi1.0o">organismo</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      La tuta persono:
      <ekz>
        la <tld/>ogardistoj<fnt>Z</fnt>
        <klr>(gardantaj la personon de regnestro)</klr>;
      </ekz>
      <ekz>
        ili vivas per unu animo en du <tld/>oj
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        li &ccirc;eestis per propra <tld/>o.
      </ekz>
    </dif>
    <trd lng="cs">t&ecaron;lo</trd>
    <trd lng="en">body</trd>
  </snc>
  <snc>
    <dif>
      &Ccirc;iu aro da materiaj eroj:
      <ekz>
        &ccirc;iuj <tld/>oj havas dimension kaj
        pezon;
      </ekz>
      <ekz>
        organika, neorganika <tld/>o;
      </ekz>
      <ekz>
        solida, fluida, gasa, simpla, komponita <tld/>o
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="materi.0o">materio</ref>,
      <ref cel="sxtof.0o">&scirc;tofo</ref>,
      <ref cel="substa.0o">substanco</ref>,
      <ref cel="elemen.0o">elemento</ref>.
    </refgrp>
    <trd lng="cs">t&ecaron;lo</trd>
    <trd lng="en">body</trd>
  </snc>
  <snc mrk="korp.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>EKV</bib><lok>&para;233</lok></fnt>
    <dif>
      Tia <ref cel="unu.0hava">unuhava</ref>
      <ref tip="super" cel="ring.0o.MAT">ringo</ref>
      (<em>E</em>,T,&#xd7;), ke krom la
      <ref cel="neuxtr.0aelemento">ne&ubreve;tra elemento</ref> de T &ccirc;iuj
      aliaj elementoj estas
      <ref cel="neuxtr.0igebla.MAT">ne&ubreve;trigeblaj</ref> rilate al &#xd7;:
      <ekz>
        la <tld/>o de racionalaj nombroj.
      </ekz>
    </dif>
    <rim>
      La&ubreve; kelkaj la komuteco de operacio &#xd7; estas deviga en la
      difino de <tld/>o. Aliaj preferas nomi <ctl>kampoj</ctl>
      la <ref cel="komut.0eca.ringo">komutecajn</ref> <tld/>ojn.
    </rim>
    <trd lng="de">K&ouml;rper</trd>
    <trd lng="en">field</trd>
    <trd lng="pl">cia&lstroke;o</trd>
    <trd lng="hu">sz&aacute;mtest</trd>
  </snc>
  <snc mrk="korp.0o.solido">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="stl">ARK</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 25</lok></fnt>
    <ref tip="dif" cel="solid.0o.geometrio">solido.</ref>
    <trdgrp lng="de">
      <trd>K&ouml;rper</trd>,
      <trd>geometrischer <ind>K&ouml;rper</ind></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>geometric <ind>body</ind></trd>,
      <trd>solid</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>solide<klr tip="ind"> (figure g&eacute;om&eacute;trique)</klr></trd>,
      <trd>volume<klr tip="ind"> (figure g&eacute;om&eacute;trique)</klr></trd>,
      <trd>corps g&eacute;om&eacute;trique</trd>
    </trdgrp>
    <trd lng="pl">bry&lstroke;a</trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_je;&c_l;&c_o;</trd>,
      <trd>&c_g;&c_je;&c_o;&c_m;&c_je;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; <ind>&c_t;&c_je;&c_l;&c_o;</ind></trd>
    </trdgrp>
  </snc>
  <trd lng="fr">corps</trd>
  <trd lng="hu">test</trd>
  <trd lng="ru">&c_t;&c_je;&c_l;&c_o;</trd>
</drv>

<drv mrk="korp.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a puno<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>corporel</trd>,
    <trd>physique</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>testi</trd>,
    <trd>fizikai</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_l;&c_je;&c_s;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="korp.en0igi">
  <kap>(en)<tld/>igi</kap>
  <snc>
    <dif>
      Fari, ke io havu <tld/>on; prezenti ion abstraktan sub <tld/>a, konkreta
      formo:
      <ekz>
        skulptisto <tld/>igas siajn revojn;
      </ekz>
      <ekz>
        lingvo internacia en<tld/>igas en si homfratigan ideon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li sola en<tld/>igas la tutan aktivecon de l' grupo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>concr&eacute;tiser</trd>,
    <trd>incarner</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_p;&c_l;&c_o;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_o;&c_p;&c_l;&c_o;&c_shch;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="korp.en0igxi">
  <kap>(en)<tld/>i&gcirc;i</kap>
  <snc>
    <ekz>
      La katolikoj kredas, ke Dio en<tld/>i&gcirc;is en Jesuo;
    </ekz>
    <ekz>
      tiuj fremdaj esprimoj en<tld/>i&gcirc;is en la lingvo
    </ekz>
    <refgrp tip="vid">
      <ref cel="person">personigi</ref>,
      <ref cel="karn">enkarnigi</ref>,
      <ref cel="asimil">asimili&gcirc;i</ref>,
      <ref cel="konkre">konkreti&gcirc;i</ref>.
    </refgrp>
  </snc>
  <trd lng="fr">s'<ind>incarner</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_p;&c_l;&c_o;&c_t;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_o;&c_p;&c_l;&c_o;&c_shch;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="korp.sub0o">
  <kap>sub<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(de <ref tip="super" cel="korp.0o.MAT"><tld/>o</ref>
      <em>K</em>)</klr>
      &Ccirc;iu unuhava <ref tip="super" cel="ring.sub0o">subringo</ref>
      de &gcirc;i, kiu subringo estas anka&ubreve; <tld/>o.
    </dif>
  </snc>
  <adm>
    Fonto trovenda, sed la formo estas tute regula. <aut>MB</aut>
  </adm>
  <trd lng="de">Unterk&ouml;rper</trd>
  <trd lng="en">subfield</trd>
  <trd lng="fr">sous-corps</trd>
  <trd lng="pl">podcia&lstroke;o</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_p;&c_o;&c_l;&c_je;</trd>
</drv>

<drv mrk="korp.super0o">
  <kap>super<tld/>o, supera <tld/>o<fnt><bib>EKV</bib><lok>&para;234</lok></fnt></kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(de <ref tip="super" cel="korp.0o.MAT"><tld/>o</ref>
      <em>K</em>)</klr>
      Tia <tld/>o, ke <em>K</em> estas <ref cel="korp.sub0o">sub<tld/>o</ref>
      de &gcirc;i.
    </dif>
    <rim>
      La naciaj lingvoj konas por &ccirc;i tiu nocio sinonimon signifantan
      <ctl>vastiga&jcirc;o</ctl>. Efektive, la super<tld/>o ofte rezultas
      el konscia klopodo konstrui ion pli vastan ol la baza korpo. Tial
      trovi&gcirc;as en <fnt><bib>EKV</bib><lok>&para;234</lok></fnt> la
      termino <ctl><tld/>opluiga&jcirc;o</ctl> (cetere kun iom pli abstrakta
      difino, nome: paro de korpoj). La tempa nuanco entenata en
      <ctl>pluigi</ctl> igas tamen la terminon iom mistrafa.
    </rim>
  </snc>
  <adm>
    Fonto trovenda, sed la formo estas tute regula. Notu, ke EKV
    diras "supera korpo", sed la kunmetajxo
    per "super" sxajnas al mi malpli problema.
    <aut>MB</aut>
  </adm>
  <trdgrp lng="de">
    <trd>Oberk&ouml;rper</trd>,
    <trd>K&ouml;rpererweiterung</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>overfield</trd>,
    <trd>extension field</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>sur-corps</trd>,
    <trd>extension d'un corps</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>nadcia&lstroke;o</trd>,
    <trd>rozszerzenie<klr tip="ind"> (cia&lstroke;a)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_d;&c_p;&c_o;&c_l;&c_je;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_i;&c_r;&c_je;&c_n;&c_i;&c_je;<klr tip="ind"> (&c_p;&c_o;&c_d;&c_p;&c_o;&c_l;&c_ja;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="korp.galeza0o">
  <kap>galeza <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      Finia <ref tip="super" cel="korp.0o"><tld/>o</ref>:
      <ekz>
        la kardinalo de galeza <tld/>o estas potenco de primo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="galez.0o">Galezo</ref>.
    </refgrp>
  </snc>
  <adm>
    Fonto trovenda. <aut>MB</aut>
  </adm>
  <trd lng="de">Galoisfeld</trd>
  <trd lng="en">Galois['s] <ind>field</ind></trd>
  <trd lng="fr">corps de Galois</trd>
  <trd lng="pl">cia&lstroke;o sko&nacute;czone</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_je; &c_G;&c_a;&c_l;&c_u;&c_a;</trd>
</drv>

<drv mrk="korp.galezasuper0o">
  <kap>galeza super<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(de <ref tip="super" cel="korp.0o.MAT"><tld/>o</ref>
      <em>K</em>)</klr>
      Tia <ref tip="super" cel="korp.super0o.MAT">super<tld/>o</ref>
      <em>K'</em> de <em>K</em>, ke &gcirc;i havas finian
      <ref cel="grad.0o.superkorpo">gradon</ref> kaj ke la
      <ref cel="korp.invarianto0o">invariantokorpo</ref> de la
      <ref cel="grup.galeza0o.de_superkorpo">galeza grupo</ref>
      Gal(<em>K'</em>/<em>K</em>) egalas al <em>K</em>.
    </dif>
    <refgrp tip="vid">
      <ref cel="galez.0o">Galezo</ref>.
    </refgrp>
  </snc>
  <adm>
    Fonto trovenda. <aut>MB</aut>
  </adm>
  <trd lng="de">galoissche <ind>Erweiterung</ind></trd>
  <trd lng="en">Galois['s] <ind>extension</ind></trd>
  <trd lng="fr">extension galoisienne</trd>
  <trd lng="pl">rozszerzenie Galois</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_sh;&c_i;&c_r;&c_je;&c_n;&c_i;&c_je; &c_G;&c_a;&c_l;&c_u;&c_a;</trd>
</drv>

<drv mrk="korp.invarianto0o">
  <kap>invarianto<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(de grupo de <ref cel="auxtom2.0o">a&ubreve;tomorfioj</ref> super
      <ref tip="super" cel="korp.0o.MAT"><tld/>o</ref> <em>K</em>)</klr>
      La sub<tld/>o de <em>K</em>, enhavanta la
      <ref cel="invari.0o">invariantojn</ref> komunaj al &ccirc;iuj
      a&ubreve;tomorfioj.
    </dif>
  </snc>
  <adm>
    Fonto trovenda. <aut>MB</aut>
  </adm>
  <trd lng="de">Invariantenk&ouml;rper</trd>
  <trd lng="en">fixed <ind>field</ind></trd>
  <trd lng="fr">corps des invariants</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_je; &c_i;&c_n;&c_v;&c_a;&c_r;&c_i;&c_a;&c_n;&c_t;&c_o;&c_v;</trd>
</drv>

<drv mrk="korp.radika0o">
  <kap>radika <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(de <ref cel="polino.0o">polinomo</ref> <em>P</em> super
      <ref tip="super" cel="korp.0o.MAT"><tld/>o</ref> <em>K</em>)</klr>
      La plej malgranda super<tld/>o de <em>K</em>, enhavanta &ccirc;iujn
      <ref cel="radik.0o.polinomo">radikojn</ref> de <em>P</em>:
      <ekz>
        la radika <tld/>o de la reela polinomo <em>X</em><sup>2</sup>+<em>1</em>
        estas la korpo de kompleksoj.
      </ekz>
    </dif>
  </snc>
  <adm>
    Fonto trovenda. <aut>MB</aut>
  </adm>
  <trd lng="de">W&uuml;rzelk&ouml;rper<klr tip="ind"> (eines Polynoms)</klr></trd>
  <trd lng="en">root <ind>field</ind><klr tip="ind"> (of a polynomial)</klr></trd>
  <trd lng="fr">corps des racines<klr tip="ind"> (d'un polyn&ocirc;me)</klr></trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_je; &c_r;&c_a;&c_z;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_ja;<klr tip="ind"> (&c_m;&c_n;&c_o;&c_g;&c_o;&c_ch;&c_l;&c_je;&c_n;&c_a;)</klr></trd>
</drv>
</art>
