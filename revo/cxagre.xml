<drv mrk="cxagre.0a">
  <kap><tld/>a</kap>
  <snc mrk="cxagre.0a.KOMUNE">
    <dif>
      Sentanta, montranta a&ubreve; ka&ubreve;zanta <tld/>on:
      <ekz>
        kia &ccirc;agrena afero
        <fnt>
          <aut>H. C. Andersen trad. L. L. Zamenhof</aut>,
          <vrk>Fabeloj, Dua Parto</vrk>,
          <lok>Pa&scirc;tisto de porkoj</lok>
        </fnt>!
      </ekz>
      <ekz>
        li estis tre <tld/>a pro la penso disi&gcirc;i for de bona mastro
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a rido
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        via mieno &scirc;ajnas &ccirc;agrena
        <fnt>
          <aut>H. Vallienne</aut>,
          <vrk>Kastelo de Prelongo</vrk>,
          <lok>&ccirc;apitro 15</lok>
        </fnt>;
      </ekz>
      <ekz>
        la grafino estis fortege <tld/>a, eksciante ke en iaj okazoj &scirc;i devos peti la
        bonvolon de sia edzo
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekkvina</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
