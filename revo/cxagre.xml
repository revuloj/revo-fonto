<drv mrk="cxagre.0o">
  <kap><tld/>o</kap>
  <snc mrk="cxagre.0o.KOMUNE">
    <dif>
      Sento kaj stato de iu <tld/>ita:
      <ekz>
        li ru&gcirc;i&gcirc;is de <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e <tld/>o de la koro la spirito estas malgaja
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 15:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        grandan &gcirc;ojon akompanas grandaj <tld/>oj
        <fnt><bib>Far2</bib><lok>&ccirc;ap. 21</lok></fnt>;
      </ekz>
      <ekz>
        havi la samajn &gcirc;ojojn kaj <tld/>ojn
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiam <nom>Sanbalat</nom> a&ubreve;dis, ke ni konstruas la muregon, li koleris kaj havis grandan
        <tld/>on, kaj li mokis la Judojn
        <fnt>
          <bib>MT</bib>
          <lok>Ne&hcirc;emja 4:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        filo malsa&gcirc;a estas <tld/>o por sia patro kaj mal&gcirc;ojo por sia patrino
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 17:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        kontra&ubreve; tiu homo <klr>[&#8230;]</klr> &scirc;i sentas ian <tld/>on, <klr>[&#8230;]</klr> en sia
        spirito &scirc;i akuzas lin parte pri tio, kion &scirc;i suferas
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        Pa&ubreve;lo, pro am<tld/>o frua eterne restis fra&ubreve;lo
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
          <lok>Konsolado</lok>
        </fnt>;
      </ekz>
      <ekz>
        beda&ubreve;ro kaj <tld/>o &scirc;uldon ne kovras
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        por unu festeno, por alia <tld/>o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="cxagre.0igxo"><tld/>i&gcirc;o</ref>
    <refgrp tip="vid">
       <ref cel="koler.0o">kolero</ref>,
       <ref cel="humor">malbonhumoro</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_s;&c_m;&c_u;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_k;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_z;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_d;&c_o;&c_s;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_o;&c_g;&c_o;&c_r;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="ca">
