<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kokcin1.xml,v 1.13 2022/12/15 08:34:17 revo Exp $">
<kap>
  <rad>kokcineled</rad>/oj
</kap>
<drv mrk="kokcin1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="koleop.0oj">koleopteroj</ref>
      (<trd lng="la">Coccinellidae</trd>):
      <ekz>
        <ref tip="prt" cel="kokcin.0o">kokcineloj</ref>
        estas genro de duonglobformaj insektoj el la ordo de
        koleopteroj,
        familio de <tld/>oj, ofte kun granda nombro da rondaj makuloj
        sur la elitroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=9459"
              >Kokcinelo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_r;&c_o;&c_w;&c_k;&c_ib; &c_b;&c_o;&c_zh;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>,
    <trd>&c_z;&c_ja;&c_z;&c_ju;&c_l;&c_mol;&c_k;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja; &c_k;&c_a;&c_z;&c_u;&c_r;&c_a;&c_k;)</klr></trd>
  </trdgrp>
  <trd lng="bg">&c_k;&c_a;&c_l;&c_i;&c_n;&c_k;&c_i;</trd>
  <trd lng="de">Marienk&auml;fer<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="es">Coccin&eacute;lidos</trd>
  <trd lng="fr">coccinellid&eacute;s</trd>
  <trd lng="hu">katicabog&aacute;rf&eacute;l&eacute;k</trd>
  <trd lng="nl">lieveheersbeestjes<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pl">biedronkowate</trd>     
  <trd lng="ro">g&abreve;rg&abreve;ri&#539;ele</trd>
  <trd lng="ru">&c_b;&c_o;&c_zh;&c_mol;&c_i; &c_k;&c_o;&c_r;&c_o;&c_v;&c_k;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sv">nyckelpigor</trd>
</drv>
</art>
