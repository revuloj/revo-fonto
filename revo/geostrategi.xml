  <trdgrp lng="zh">
    <trd>&#25112;&#30053;&#x5B66; <pr>zh&#xE0;nl&#xFC;&#xE8;xu&#xE9;</pr></trd>,
    <trd>&#22320;&#x7DE3;&#25136;&#30053; <pr>d&#xEC;yu&#xE1;nzh&#xE0;nl&#xFC;&#xE8;</pr></trd>
  </trdgrp>
</drv>
