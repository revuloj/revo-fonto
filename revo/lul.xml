<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lul.xml,v 1.30 2024/04/10 07:25:27 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lul</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="lul.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="lul.0i.MOV">
    <dif>
      <ref tip="super" cel="dorm.0igi.ripozigi">Dormigi</ref>, trankviligi per balanceta movado,
      precipe infaneton:
      <ekz>
        la maljuna homo sidis kaj <tld/>is la infanon, kaj la patrino sidi&gcirc;is sur la
        se&gcirc;o tute apude
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de unu patrino</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam &ccirc;iujn jam <tld/>is la dormo kaj la princino iris en la bandomon, li saltis
        al &scirc;i kaj &scirc;in kisis
        <fnt>
          <bib>ElektFab</bib>
          <lok>La ora birdo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i lasis sin <tld/>i en la brakoj de sia amato, redonante liajn karesojn
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekdua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la egalritmaj pa&scirc;oj de la soldatoj <tld/>is lin al dormo
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malvarmo de la nokto <tld/>us vin en la morton
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="es">
      <trd>acunar</trd>,
      <trd>mecer</trd>
    </trdgrp>
  </snc>
  <snc mrk="lul.0i.balanceti">
    <dif>
      Harmonie <ref tip="vid" cel="balanc1.0i">balanceti</ref>:
      <ekz>
        la ventoj ludis kun ili, <klr>[&#8230;]</klr> portis iliajn flugdrakojn al la altoj
        kaj <tld/>is ilin mem sur la suproj de la arbokronoj
        <fnt>
          <bib>HsT</bib>
          <lok>La nepo</lok>
        </fnt>;
      </ekz>
      <ekz>
        foje-foje orfolio <tld/>as sin malsupren por fine halti anta&ubreve; la piedo de la
        promenanto
        <fnt>
          <bib>LGA</bib>
          <lok>I.</lok>
        </fnt>;
      </ekz>
      <ekz>
        urbo ku&scirc;anta apud la Mezamaro, kies ondoj <tld/>is plej gravajn civilizaciojn
        <fnt>
          <bib>DKM</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ridetas kaj lulas sian grandan, muskolan korpon
        <fnt>
          <bib>VRA</bib>
          <lok>Eyvind Johnson</lok>
        </fnt>;
      </ekz>
      <ekz>
        friska vento <tld/>is la bran&ccirc;ojn de la arboj en la parko
        <fnt>
	<aut>Julian Modest</aut>
	<vrk><url ref="&Monato;/2001/007215.php">La peko de Ros</url></vrk>
	<bib>Monato</bib>
	<lok>2001/01, p. 24</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Pacifika maro komencis <tld/>i ilin, tien-reen inter la &scirc;tonoj
        <fnt>
          <bib>VRA</bib>
          <lok>Harry Martinson</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="es">mecer</trd>
  </snc>
  <snc mrk="lul.0i.kvietigi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="kviet.0igi">Kvietigi</ref>, kvaza&ubreve;dormigi:
      <ekz>
        la amo de Amalio <tld/>as lin en la ventegoj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        poste <tld/>os vin la dol&ccirc;a forgeso pri la anta&ubreve;aj embarasoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la revo, kiu povis <tld/>i nian doloron, fari&gcirc;is tute neefektivigebla
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        senlima feli&ccirc;o de ripozo <tld/>is miajn ekscititajn nervojn
        <fnt>
          <bib>VaK</bib>
          <lok>Dekoka &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="es">
      <trd>calmar</trd>,
      <trd>arrullar</trd>,
      <trd>adormecer</trd>,
      <trd>sosegar</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_l;&c_y;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_l;&c_ju;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_u;&c_sh;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_l;&c_ju;&c_l;&c_je;&c_ja;</trd>
  <trd lng="br">luskella&ntilde;</trd>
  <trd lng="cs">kol&eacute;bat</trd>
  <trd lng="de">wiegen</trd>
  <trdgrp lng="en">
    <trd>lull</trd>,
    <trd>cradle</trd>
  </trdgrp>
  <trd lng="fr">bercer</trd>
  <trdgrp lng="hu">
    <trd>ringat</trd>,
    <trd>elringat</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x63FA;&#12377;&#x308B;  <pr>&#12422;&#12377;&#x308B;</pr></trd>,
    <trd>&#x63FA;&#12425;&#12377;  <pr>&#12422;&#12425;&#12377;</pr></trd>,
    <trd>&#x306A;&#12384;&#12417;&#x308B;</trd>
  </trdgrp>
  <trd lng="nl">wiegen</trd>
  <trdgrp lng="pl">
    <trd>ko&lstroke;ysa&cacute;</trd>,
    <trd>lula&cacute;</trd>,
    <trd>buja&cacute;</trd>
  </trdgrp>
  <trd lng="pt">embalar</trd>
  <trdgrp lng="ro">
    <trd>leg&abreve;na</trd>,
    <trd>calma</trd>,
    <trd>pluti</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_k;&c_a;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_b;&c_a;&c_ju;&c_k;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_a;&c_ju;&c_k;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hojda&tcaron;</trd>,
    <trd>k&yacute;va&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_k;&c_o;&c_l;&c_i;&c_s;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_g;&c_o;&c_j;&c_d;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_o;&c_l;&c_i;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="lul.0ilo">
  <kap><ofc>*</ofc><tld/>ilo <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="&AdE;/fundamento/universala_vortaro.html#av-5095">lul'il'</url></lok></fnt></kap>
  <snc mrk="lul.0ilo.MEBL">
    <dif>
      Infan<ref tip="super" cel="lit.0o.MEBL">lito</ref> balancebla,
      per kiu oni povas <tld/>i:
      <ekz>
        tutajn tagojn sidis &ccirc;e la <tld/>ilo de sia filo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i kantis pri la &ccirc;agrenoj, kiuj persekutas la homan genton
        <ind>de la <tld/>ilo &gcirc;is la morto</ind>
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
        <trd lng="de">von der <ind>Wiege</ind> bis zur Bahre</trd>
      </ekz>
      <ekz>
        tio estis lia poeta talento, kiun Dio donace metis al li en la <tld/>ilon
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        li sin direktis al la komoda tirkesto aliformigita en <tld/>ilon, kaj longe
        rigardadis la esta&jcirc;eton
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="segx.balanco0o">lulse&gcirc;o</ref>
    </refgrp>
    <bld
    lok="&WCU;/thumb/3/33/Gustav_Adolf_Hessl_-_An_der_Wiege_-_476_-_Belvedere.jpg/360px-Gustav_Adolf_Hessl_-_An_der_Wiege_-_476_-_Belvedere.jpg"
    prm="PD">Gustav Adolf Hessl: &Ccirc;e La <tld lit="L"/>ilo
        <fnt>
          <aut>Gustav August Hessl, https://digital.belvedere.at/objects/6215/an-der-wiege&nbsp;</aut>
          <lok><url
          ref="&WCW;/File:Gustav_Adolf_Hessl_-_An_der_Wiege_-_476_-_Belvedere.jpg">&WCW;/File:Gustav_Adolf_Hessl_-_An_der_Wiege_-_476_-_Belvedere.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="ru">&c_l;&c_ju;&c_l;&c_mol;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="lul.0ilo.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Naski&gcirc;ejo, kovejo:
      <ekz>
        Egipto, <klr>[&#8230;]</klr> <tld/>ilo de la civilizacio
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        Germanujo <klr>[&#8230;]</klr> estas la <tld/>ilo de la ideo de lingvo internacia
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en Dresden en la 17a de a&ubreve;gusto
          1908</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_y;&c_s;&c_k;&c_a;</trd>
  <trd lng="bg">&c_l;&c_ju;&c_l;&c_k;&c_a;</trd>
  <trd lng="br">kavell<klr tip="ind"> (gwele)</klr></trd>
  <trd lng="cs">kol&eacute;bka</trd>
  <trd lng="de">Wiege</trd>
  <trd lng="en">cradle</trd>
  <trd lng="es">cuna</trd>
  <trd lng="fr">berceau</trd>
  <trd lng="hu">b&ouml;lcs&odblac;</trd>
  <trdgrp lng="ja">
    <trd>&#12422;&#x308A;&#x304B;&#12372;</trd>,
    <trd>&#x767A;&#31077;&#22320;  <pr>&#x306F;&#12387;&#12375;&#12423;&#12358;&#12385;</pr></trd>,
    <trd>&#x53D7;&#12369;&#x53F0;  <pr>&#12358;&#12369;&#12384;&#12356;</pr></trd>,
    <trd>&#x67B6;&#x53F0;  <pr>&#x304B;&#12384;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">wieg</trd>
  <trdgrp lng="pl">
    <trd>kolebka</trd>,
    <trd>ko&lstroke;yska</trd>
  </trdgrp>
  <trd lng="pt">ber&ccedil;o</trd>
  <trd lng="ro">leag&abreve;n </trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_y;&c_b;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">kol&iacute;ska</trd>
  <trd lng="uk">&c_k;&c_o;&c_l;&c_i;&c_s;&c_k;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#25622;&#x7C43; <pr>y&#xE1;ol&#xE1;n</pr></trd>,
    <trd>&#25671;&#x7BEE; <pr>y&#xE1;ol&#xE1;n</pr></trd>
  </trdgrp>
</drv>

</art>
