<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lul</rad>/i
</kap>
<drv mrk="lul.0i">
  <kap>
    <ofc>*</ofc><tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Balanceti infaneton por &gcirc;in dormigi:
      <ekz>
        <tld/>i iun al la dormo<fnt>B</fnt>.
      </ekz>
    </dif>
    <trd lng="es">mecer</trd>
  </snc>
  <snc>
    <dif>
      Balanceti:
      <ekz>
        la ondoj sen&ccirc;ese pla&ubreve;detadis <tld/>e, harmonie;
      </ekz>
      <ekz>
        <tld/>a pa&scirc;ado.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kvietigi; kvaza&ubreve;dormigi:
      <ekz>
        <tld/>i sian &ccirc;agrenon;
      </ekz>
      <ekz>
        <tld/>i sin en revoj
        <klr>(volonte kontenti&gcirc;i per nuraj esperoj)</klr>.
      </ekz>
    </dif>
    <trdgrp lng="es">
      <trd>calmar</trd>,
      <trd>arrullar</trd>,
      <trd>adormecer</trd>,
      <trd>sosegar</trd>
    </trdgrp>
  </snc>
  <trd lng="en">lull</trd>
  <trd lng="nl">wiegen</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_k;&c_a;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_b;&c_a;&c_ju;&c_k;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_a;&c_ju;&c_k;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="lul.0ilo">
  <kap>
    <tld/>ilo
  </kap>
  <snc>
    <dif>
      Infanlito balancebla, per kiu oni povas <tld/>i.
    </dif>
    <trd lng="ru">&c_l;&c_ju;&c_l;&c_mol;&c_k;&c_a;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Naski&gcirc;ejo, kovejo:
      <ekz>
        Egiptujo, <tld/>ilo de la civilizacio<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">cuna</trd>
  <trd lng="nl">wieg</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_y;&c_b;&c_je;&c_l;&c_mol;</trd>
</drv>

<drv mrk="lul.0kanto">
  <kap>
    <tld/>kanto
  </kap>
  <uzo tip="klr">(p. f.)</uzo>
  <snc>
    <dif>
      Murmura kanto por dormigi infanon, kvietigi doloron.
    </dif>
  </snc>
  <trd lng="es">nana</trd>
  <trd lng="nl">wiegelied</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_y;&c_b;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_ja; <klr>(&c_p;&c_je;&c_s;&c_n;&c_ja;)</klr></trd>
</drv>

<drv mrk="lul.0segxo">
  <kap>
    <tld/>se&gcirc;o
  </kap>
  <snc>
    <dif>
      Se&gcirc;o en kiu oni povas sin balanci.
    </dif>
  </snc>
  <trd lng="es">mecedora</trd>
  <trd lng="nl">schommelstoel</trd>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_s;&c_l;&c_o;-&c_k;&c_a;&c_ch;&c_a;&c_l;&c_k;&c_a;</trd>
</drv>
</art>
