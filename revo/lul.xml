<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lul.xml,v 1.21 2020/02/20 00:10:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lul</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="lul.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="lul.0i.MOV">
    <dif>
      Balanceti infaneton por &gcirc;in dormigi:
      <ekz>
        <tld/>i iun al la dormo<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="es">
      <trd>acunar</trd>,
      <trd>mecer</trd>
    </trdgrp>
  </snc>
  <snc mrk="lul.0i.balanceti">
    <dif>
      <ref tip="vid" cel="balanc1.0i">Balanceti</ref>:
      <ekz>
        la ondoj sen&ccirc;ese pla&ubreve;detadis <tld/>e, harmonie;
      </ekz>
      <ekz>
        <tld/>a pa&scirc;ado.
      </ekz>
    </dif>
    <trd lng="es">mecer</trd>
  </snc>
  <snc mrk="lul.0i.kvietigi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kvietigi, kvaza&ubreve;dormigi:
      <ekz>
        <tld/>i sian &ccirc;agrenon;
      </ekz>
      <ekz>
        <tld/>i sin en revoj
        <klr>(volonte kontenti&gcirc;i per nuraj esperoj)</klr>.
      </ekz>
    </dif>
    <trdgrp lng="es">
      <trd>calmar</trd>,
      <trd>arrullar</trd>,
      <trd>adormecer</trd>,
      <trd>sosegar</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_l;&c_y;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_l;&c_ju;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_l;&c_ju;&c_l;&c_je;&c_ja;</trd>
  <trd lng="br">luskella&ntilde;</trd>
  <trd lng="cs">kol&eacute;bat</trd>
  <trd lng="de">wiegen</trd>
  <trdgrp lng="en">
    <trd>lull</trd>,
    <trd>cradle</trd>
  </trdgrp>
  <trd lng="fr">bercer</trd>
  <trdgrp lng="hu">
    <trd>ringat</trd>,
    <trd>elringat</trd>
  </trdgrp>
  <trd lng="nl">wiegen</trd>
  <trdgrp lng="pl">
    <trd>ko&lstroke;ysa&cacute;</trd>,
    <trd>lula&cacute;</trd>,
    <trd>buja&cacute;</trd>
  </trdgrp>   
  <trd lng="pt">embalar</trd>
  <trdgrp lng="ro">
    <trd>leg&abreve;na</trd>,
    <trd>calma</trd>,
    <trd>pluti</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_k;&c_a;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_b;&c_a;&c_ju;&c_k;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_a;&c_ju;&c_k;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hojda&tcaron;</trd>,
    <trd>k&yacute;va&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="lul.0ilo">
  <kap><ofc>*</ofc><tld/>ilo <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url
  ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5095">lul'il'</url></lok></fnt></kap>
  <snc mrk="lul.0ilo.MEBL">
    <dif>
      Infan<ref tip="super" cel="lit.0o.MEBL">lito</ref> balancebla, 
      per kiu oni povas <tld/>i.
    </dif>
    <trd lng="ru">&c_l;&c_ju;&c_l;&c_mol;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="lul.0ilo.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Naski&gcirc;ejo, kovejo:
      <ekz>
        Egiptujo, <tld/>ilo de la civilizacio<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_y;&c_s;&c_k;&c_a;</trd>
  <trd lng="bg">&c_l;&c_ju;&c_l;&c_k;&c_a;</trd>
  <trd lng="br">kavell<klr tip="ind"> (gwele)</klr></trd>
  <trd lng="cs">kol&eacute;bka</trd>
  <trd lng="de">Wiege</trd>
  <trd lng="en">cradle</trd>
  <trd lng="es">cuna</trd>
  <trd lng="fr">berceau</trd>
  <trd lng="hu">b&ouml;lcs&odblac;</trd>
  <trd lng="nl">wieg</trd>
  <trdgrp lng="pl">
    <trd>kolebka</trd>,
    <trd>ko&lstroke;yska</trd>
  </trdgrp>
  <trd lng="pt">ber&ccedil;o</trd>
  <trd lng="ro">leag&abreve;n </trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_y;&c_b;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">kol&iacute;ska</trd>
</drv>
</art>
