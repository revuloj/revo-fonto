<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: reporter.xml,v 1.6 2019/10/20 08:10:18 revo Exp $">
<kap>
   <rad>reporter</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="reporter.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="reporter.0o.INF">
    <dif>
      <ref tip="dif" cel="raport.0isto.INF">Raportisto</ref>:
      <ekz>
       <tld/>o de la perioda gazeto <nom>Stern</nom>
       <fnt>
         <aut>hawibo</aut>,
         <vrk>la vera viza&gcirc;o...</vrk>,
         <lok><url ref="http://groups.google.be/group/soc.culture.esperanto?hl=eo">
         soc.culture.esperanto</url>, </lok>
       </fnt>;
      </ekz>
      <ekz>
       &leftquot;nia unua prioritato estos savi vivojn,&rightquot;
       Prezidento <nom>Rafael Correa</nom> diris al <tld/>oj
       <fnt>
         <aut>-</aut>,
         <vrk>Gravegaj inundoj en Ekvadoro</vrk>,
         <lok><url ref="http://www.eventeo.net/web/">
         eventeo</url>, 2008-02-21</lok>
       </fnt>;
      </ekz>
      <ekz>
        anka&ubreve; rivel-<tld/>oj de Monato ne trovis, kiel forte ordonite de Bu&scirc;o, la
        ka&scirc;itajn amasmurdajn armilojn de iu satana Sadamo
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2004/008492.html">Du mil kvar</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="jxurna.0isto">&jcirc;urnalisto</ref>
  </snc>
  <trd lng="cs">report&eacute;r</trd>
  <trd lng="de">Reporter</trd>
  <trdgrp lng="sk">
    <trd>report&eacute;r</trd>,
    <trd>spravodajca</trd>
  </trdgrp>
</drv>
</art>

