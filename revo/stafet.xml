<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>stafet</rad>/o
</kap>
<drv mrk="stafet.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Rajdanto kiu portas ordonojn a&ubreve; leterojn.
    </dif>
    <refgrp tip="sin">
      <ref cel="kurier.0o">kuriero</ref>,
      <ref cel="kur">kuristo</ref>.
    </refgrp>
    <trd lng="de">Meldereiter</trd>
    <trd lng="nl">koerier</trd>
    <trdgrp lng="ru">
      <trd>&c_g;&c_o;&c_n;&c_je;&c_c;</trd>,
      <trd>&c_k;&c_u;&c_r;&c_mol;&c_je;&c_r;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">SPO</uzo>
    <dif>
      Sporta konkuro la&ubreve; maniero de la antikva kuriera servo.
    </dif>
    <trdgrp lng="de">
      <trd>Stafette</trd>,
      <trd>Staffel</trd>
    </trdgrp>
    <trd lng="nl">estafette</trd>
    <trd lng="ru">&c_e;&c_s;&c_t;&c_a;&c_f;&c_je;&c_t;&c_a;</trd>
  </snc>
</drv>
</art>
