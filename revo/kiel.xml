  <trd lng="de">wie <klr tip="amb">(Art und Weise)</klr></trd>
      <trdgrp lng="es">
        <trd>c&oacute;mo</trd>,
        <trd>como</trd>,
        <trd>de qu&eacute; <ind>forma</ind></trd>,
        <trd>de <ind>forma</ind> que</trd>,
        <trd>de qu&eacute; <ind>manera</ind></trd>,
