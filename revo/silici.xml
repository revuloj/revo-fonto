<kap>
  <ofc>9</ofc><rad>silici</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="silici.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <mlg kod="IUPAC">Si</mlg>
  <snc mrk="silici.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="14">elemento</ref>
      kun atomnumero 14, atompezo 28,086;
      <ref tip="super" cel="metalo.0o">metaloido</ref>:
      <ekz>
        topinamburo <klr>[&#8230;]</klr> enhavas anka&ubreve; pliajn biologie
        aktivajn substancojn, mineralsubstancojn (fero, <tld/>o, zinko,
        magnezio, <klr>[&#8230;]</klr>)
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2012/010553.php">Topinamburo &ccirc;u
          herba&ccirc;o?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        argilo estas glueca knedebla grundo, konsistanta el aluminio kaj
        <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=119">Argilo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_e;&c_m;&c_n;&c_ib;&c_j;</trd>
  <trd lng="bg">&c_s;&c_i;&c_l;&c_i;&c_c;&c_i;&c_j;</trd>
  <trd lng="br">silisiom</trd>
  <trd lng="ca">silici</trd>
  <trd lng="de">Silizium</trd>
  <trd lng="en">silicon</trd>
  <trd lng="es">silicio</trd>
  <trd lng="fa">&a_s;&f_y;&a_l;&f_y;&a_s;&f_y;&a_m;</trd>
  <trd lng="fr">silicium</trd>
  <trd lng="hu">szil&iacute;cium</trd>
  <trd lng="id">silikon</trd>
  <trd lng="it">silicio</trd>
  <trd lng="ja">&#x30B1;&#x30A4;&#x7D20;  <pr>&#12369;&#12356;&#x305D;</pr></trd>
  <trd lng="la">Silicium</trd>
  <trd lng="nl">silicium</trd>
  <trd lng="pl">krzem</trd>
  <trd lng="pt">sil&iacute;cio</trd>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_m;&c_n;&c_i;&c_j;</trd>
  <trd lng="sv">kisel</trd>
  <trd lng="uk">&c_s;&c_i;&#x43B;&c_ib;&c_c;&c_ib;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#30789; <pr>gu&#x12B;</pr></trd>,
    <trd>&#x73EA; <pr>gu&#x12B;</pr></trd>
  </trdgrp>
</drv>
