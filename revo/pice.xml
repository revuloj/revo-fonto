<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pice</rad>/o
</kap>
<drv mrk="pice.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o"
      lst="voko:botnaikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="pinac.0oj">pinacoj</ref>
      (<trd lng="la">Picea</trd>)
    </dif>
    <refgrp tip="vid">
      <ref cel="abi.0o">abio</ref>,
      <ref cel="pin.0o">pino</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_je;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_ja;&c_l;&c_ib;&c_n;&c_a;</trd>
  </trdgrp>                 
  <trd lng="br">pinenn-groaz</trd>
  <trd lng="de">Fichte</trd>
  <trd lng="en">spruce</trd>
  <trd lng="fr">&eacute;pic&eacute;a</trd>
  <trd lng="hu">lucfeny&odblac;</trd>
  <trd lng="nl">spar</trd>
  <trd lng="pl">&sacute;wierk</trd>
  <trd lng="pt">p&iacute;cea</trd>
  <trd lng="ru">&c_je;&c_l;&c_mol;</trd>
</drv>
</art>
