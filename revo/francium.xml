<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: francium.xml,v 1.16 2023/11/10 06:08:00 revo Exp $">
<kap>
  <rad>francium</rad>/o,
  <var><kap><rad>franci</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>
<drv mrk="francium.0o">
  <kap><tld/>o,
    <var><kap>francio</kap></var>
  </kap>
  <mlg kod="IUPAC">Fr</mlg>
  <snc mrk="francium.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="87">elemento</ref> kun
      atomnumero 87, atompezo 223,0197,
      kies &ccirc;iuj <ref tip="super" cel="izotop.0o">izotopoj</ref> 
      estas radioaktivaj:
      <ekz>
        sciencistoj kalkulis, ke en la tuta Tero ekzistas je ajna specifa tempo entute
        malpli ol 20 atomoj da <tld/>o, pro sia radioaktiveco, &ccirc;iu atomo vivas nur
        kelkajn minutojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=28843">Franciumo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="franci1.0o">Francio (&scirc;tato)</ref>
  <trd lng="be">&c_f;&c_r;&c_a;&c_n;&c_c;&c_y;&c_j;</trd>
  <trd lng="bg">&c_f;&c_r;&c_a;&c_n;&c_c;&c_i;&c_j;</trd>
  <trd lng="br">fra&ntilde;siom</trd>
  <trd lng="ca">franci</trd>
  <trd lng="de">Francium</trd>
  <trd lng="en">francium</trd>
  <trd lng="es">francio</trd>
  <trd lng="fa">&a_f;&a_r;&a_A;&a_n;&a_s;&f_y;&a_damma;&a_m;</trd>
  <trd lng="fr">francium</trd>
  <trd lng="hu">francium</trd>
  <trd lng="id">fransium</trd>
  <trd lng="it">francio</trd>
  <trd lng="la">Francium</trd>
  <trd lng="nl">francium</trd>
  <trd lng="pl">frans</trd>
  <trd lng="pt">fr&acirc;ncio</trd>
  <trd lng="ru">&c_f;&c_r;&c_a;&c_n;&c_c;&c_i;&c_j;</trd>
  <trd lng="sv">francium</trd>
  <trd lng="sk">francium</trd>
  <trd lng="cs">francium</trd>
  <trd lng="uk">&#x444;&#x440;&#x430;&#x43D;&#x446;&#x456;&#x439;</trd>
  <trdgrp lng="zh">
    <trd>&#x9345; <pr>f&#x1CE;</pr></trd>,
    <trd>&#x9201; <pr>f&#x101;ng</pr></trd>
  </trdgrp>
</drv>
</art>

