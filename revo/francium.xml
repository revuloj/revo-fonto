<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>francium</rad>/o <fnt>francio en ReVo</fnt>
</kap>
<drv mrk="francium.0o">
  <kap><tld/>o</kap>
  <mlg>Fr</mlg>
  <snc mrk="francium.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 87, atompezo 223,0197, simbolo <frm>Fr</frm>.
      &Ccirc;iuj <ref tip="super" cel="izotop.0o">izotopoj</ref> 
      de tiu elemento estas radioaktivaj.
    </dif>
  </snc>
  <ref tip="sin" cel="franci2.0o">francio</ref>
  <trd lng="be">&c_f;&c_r;&c_a;&c_n;&c_c;&c_y;&c_j;</trd>   
  <trd lng="br">fra&ntilde;siom</trd>
  <trd lng="de">Francium</trd>
  <trd lng="en">francium</trd>
  <trd lng="es">francio</trd>
  <trd lng="fa">&a_f;&a_r;&a_A;&a_n;&a_s;&f_y;&a_damma;&a_m;</trd>
  <trd lng="fr">francium</trd>
  <trd lng="hu">francium</trd>
  <trd lng="it">francio</trd>
  <trd lng="la">Francium</trd>
  <trd lng="nl">francium</trd>
  <trd lng="pl">frans</trd>
  <trd lng="pt">fr&acirc;ncio</trd>
  <trd lng="ru">&c_f;&c_r;&c_a;&c_n;&c_c;&c_i;&c_j;</trd>
</drv>
</art>

