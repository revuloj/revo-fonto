<vortaro>
<art mrk="$Id: monopo.xml,v 1.31 2025/05/11 08:00:20 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>monopol</rad>/o <fnt><bib>JED</bib></fnt> 
</kap>
<drv mrk="monopo.0o">
  <kap><tld/>o</kap>
  <snc mrk="monopo.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Privilegio donanta al iu persono, kompanio a&ubreve; &scirc;tato
      ekskluzivan
      rajton fabriki a&ubreve; vendi ian komerca&jcirc;on a&ubreve;
      plenumi ian servon:
      <ekz>
        &scirc;tata <tld/>o pri tabako;
      </ekz>
      <ekz>
        havi <tld/>on super la merkato.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konces">koncesio</ref>,
      <ref cel="privil.0o">privilegio</ref>.
    </refgrp>
  </snc>
  <snc mrk="monopo.0o.EKON">
    <fnt><bib>IKEV</bib></fnt>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Merkata situacio, en kiu unu kompanio regas la vendan flankon kaj
      diktas la prezojn al klientoj:
      <ekz>
        registaroj ofte batalas kontra&ubreve; <tld/>firmaoj, &ccirc;ar
        <tld/>o forigas konkuradon kaj tiel tendencas
        al altigo de prezoj kaj malaltigo de la produktkvalito.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akapar">akaparo</ref>,
      <ref cel="kartel.0o">kartelo</ref>,
      <ref cel="trust.0o">trusto</ref>,
    </refgrp>
    <refgrp tip="vid">
      <ref cel="duopol.0o">duopolo</ref>,
      <ref cel="polipol.0o">polipolo</ref>,
      <ref cel="oligopol.0o">oligopolo</ref>.
    </refgrp>
  </snc>
  <snc mrk="monopo.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ekskluziva posedo:
      <ekz>
        batali pri la <tld/>o ekspluati la popolon<fnt>B</fnt>;
      </ekz>
      <ekz>
        vi ne havas la <tld/>on de la kura&gcirc;o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_a;&c_p;&c_o;&c_l;&c_ib;&c_ja;</trd>
  <trd lng="ca">monopoli</trd>
  <trd lng="cs">monopol</trd>
  <trd lng="de">Monopol</trd>
  <trd lng="es">monopolio</trd>
  <trd lng="fr">monopole</trd>
  <trdgrp lng="ja">
    <trd>&#29420;&#21344;  <pr>&#12393;&#12367;&#12379;&#12435;</pr></trd>,
    <trd>&#23554;&#22770;  <pr>&#12379;&#12435;&#12400;&#12356;</pr></trd>,
    <trd>&#21344;&#26377;  <pr>&#12379;&#12435;&#12422;&#12358;</pr></trd>,
    <trd>&#29420;&#12426;&#21344;&#12417;  <pr>&#12402;&#12392;&#12426;&#12376;&#12417;</pr></trd>
  </trdgrp>
  <trd lng="nl">monopool</trd>
  <trd lng="pl">monopol</trd>
  <trd lng="ro">monopol</trd>
  <trd lng="ru">&c_m;&c_o;&c_n;&c_o;&c_p;&c_o;&c_l;&c_i;&c_ja;</trd>
  <trd lng="sk">monopol</trd>
  <trd lng="sv">monopol</trd>
  <trd lng="tr">tekel</trd>
  <trd lng="uk">&c_m;&c_o;&c_n;&c_o;&c_p;&c_o;&c_l;&c_ib;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#22404;&#26029; <pr>l&#466;ngdu&agrave;n</pr></trd>,
    <trd>&#22751;&#26039; <pr>l&#466;ngdu&agrave;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="monopo.absoluta0o">
  <kap>absoluta <tld/>o<fnt><bib>KrDE</bib></fnt></kap>
  <snc mrk="monopo.absoluta0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <tld lit="M"/>o tiom forta, ke neniu merkata
      aganto povas &gcirc;in minaci.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_s;&c_a;&c_l;&c_ju;&c_t;&c_n;&c_a;&c_ja;
    <ind>&c_m;&c_a;&c_n;&c_a;&c_p;&c_o;&c_l;&c_ib;&c_ja;</ind></trd>,
    <trd>&c_n;&c_je;&c_a;&c_b;&c_m;&c_je;&c_zh;&c_a;&c_v;&c_a;&c_n;&c_a;&c_ja;
    <ind>&c_m;&c_a;&c_n;&c_a;&c_p;&c_o;&c_l;&c_ib;&c_ja;</ind></trd>
  </trdgrp>
  <trd lng="ca">monopoli pur</trd>
  <trdgrp lng="de">
     <trd>absolutes Monopol</trd>,
     <trd>uneingeschr&auml;nktes Monopol</trd>
  </trdgrp>
  <trd lng="en">absolute monopoly</trd>
  <trdgrp lng="es">
    <trd>monopolio absoluto</trd>,
    <trd>monopolio puro</trd>
  </trdgrp>
  <trd lng="pl">monopol absolutny</trd>
  <trd lng="ro">monopol absolut</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_b;&c_s;&c_o;&c_l;&c_ju;&c_t;&c_n;&c_a;&c_ja;
    <ind>&c_m;&c_o;&c_n;&c_o;&c_p;&c_o;&c_l;&c_i;&c_ja;</ind></trd>,
    <trd>&c_n;&c_je;&c_o;&c_g;&c_r;&c_a;&c_n;&c_i;&c_ch;&c_je;&c_n;&c_n;&c_a;&c_ja;
    <ind>&c_m;&c_o;&c_n;&c_o;&c_p;&c_o;&c_l;&c_i;&c_ja;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="monopo.duflanka0o">
  <kap>duflanka <tld/>o</kap>
  <snc mrk="monopo.duflanka0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Samtempa ekzisto de <tld/>o kaj
      <ref tip="vid" cel="monops.0o">monopsonio</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_v;&c_u;&c_h;&c_b;&c_a;&c_k;&c_o;&c_v;&c_a;&c_ja;
  <ind>&c_m;&c_a;&c_n;&c_a;&c_p;&c_o;&c_l;&c_ib;&c_ja;</ind></trd>
  <trd lng="ca">monopoli bilateral</trd>
  <trd lng="de">bilaterales Monopol</trd>
  <trd lng="es">monopolio bilateral</trd>
  <trd lng="pl">monopol dwustronny</trd>
  <trd lng="ro">monopol &icirc;n dou&abreve; sensuri</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_v;&c_u;&c_s;&c_t;&c_o;&c_r;&c_o;&c_n;&c_n;&c_ja;&c_ja;
    <ind>&c_m;&c_o;&c_n;&c_o;&c_p;&c_o;&c_l;&c_i;&c_ja;</ind></trd>,
    <trd>&c_b;&c_i;&c_l;&c_a;&c_t;&c_je;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja;
    <ind>&c_m;&c_o;&c_n;&c_o;&c_p;&c_o;&c_l;&c_i;&c_ja;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="monopo.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="monopo.0igi.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      &Gcirc;ui <tld/>on pri io:
      <ekz>
        la trusto <tld/>igas la petrolon.
      </ekz>
    </dif>
  </snc>
  <snc mrk="monopo.0igi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ekskluzive posedi, &gcirc;ui, logi al si:
      <ekz>
        la bela, juna edzino de fabele ri&ccirc;a anglo tuj <tld/>igis
        la atenton de la tuta unuaklasa pasa&gcirc;eraro
        <fnt>
          <aut>H. A. Luyken</aut>,
          <vrk>Rakonto de Maljuna Klubano</vrk>,
          <lok>Verda Mondo, 1927-10, p. 28a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>  
  <trd lng="be">&c_m;&c_a;&c_n;&c_a;&c_p;&c_a;&c_l;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">monopolitzar</trd>
  <trdgrp lng="cs">
    <trd>monopolizovat</trd>,
    <trd>zmonopolizovat</trd>
  </trdgrp>
  <trd lng="de">monopolisieren</trd>
  <trd lng="en">monopolize</trd>
  <trd lng="es">monopolizar</trd>
  <trd lng="fr">monopoliser</trd>
  <trd lng="nl">monopoliseren</trd>
  <trd lng="pl">monopolizowa&cacute;</trd>
  <trd lng="ro">monopoliza</trd>
  <trd
  lng="ru">&c_m;&c_o;&c_n;&c_o;&c_p;&c_o;&c_l;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">monopolizova&tcaron;</trd>
  <trd lng="uk">&c_m;&c_o;&c_n;&c_o;&c_p;&c_o;&c_l;&c_ib;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#22404;&#26029; <pr>l&#466;ngdu&agrave;n</pr></trd>,
    <trd>&#22751;&#26039; <pr>l&#466;ngdu&agrave;n</pr></trd>,
    <trd>&#19987;&#33829; <pr>zhu&#257;ny&iacute;ng</pr></trd>,
    <trd>&#23560;&#29151; <pr>zhu&#257;ny&iacute;ng</pr></trd>,
    <trd>&#21253;&#25597; <pr>b&#257;ol&#462;n</pr></trd>,
    <trd>&#21253;&#25900; <pr>b&#257;ol&#462;n</pr></trd>,
    <trd>&#29420;&#21344; <pr>d&uacute;zh&agrave;n</pr></trd>,
    <trd>&#29544;&#20308; <pr>d&uacute;zh&agrave;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="monopo.eksport0o">
  <kap>eksport<tld/>o</kap>
  <snc mrk="monopo.eksport0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <tld lit="M"/>o je la eksportado de varo.
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_s;&c_p;&c_a;&c_r;&c_t;&c_n;&c_a;&c_ja;
  <ind>&c_m;&c_a;&c_n;&c_a;&c_p;&c_o;&c_l;&c_ib;&c_ja;</ind></trd>
    <trdgrp lng="de">
      <trd>Ausfuhrmonopol</trd>,
      <trd>Exportmonopol</trd>
    </trdgrp>
  <trd lng="en">export monopoly</trd>
  <trd lng="pl">monopol eksportowy</trd>
  <trd lng="ro">monopolul exportului</trd>
  <trd lng="ru">&c_e;&c_k;&c_s;&c_p;&c_o;&c_r;&c_t;&c_n;&c_a;&c_ja;
  <ind>&c_m;&c_o;&c_n;&c_o;&c_p;&c_o;&c_l;&c_i;&c_ja;</ind></trd>
</drv>

<drv mrk="monopo.import0o">
  <kap>import<tld/>o</kap>
  <snc mrk="monopo.import0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <tld lit="M"/>o je la importado de varo.
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_m;&c_p;&c_a;&c_r;&c_t;&c_n;&c_a;&c_ja;
  <ind>&c_m;&c_a;&c_n;&c_a;&c_p;&c_o;&c_l;&c_ib;&c_ja;</ind></trd>
  <trd lng="cs">dovozn&iacute; monopol</trd>
    <trdgrp lng="de">
      <trd>Einfuhrmonopol</trd>,
      <trd>Importmonopol</trd>
    </trdgrp>
  <trd lng="en">import monopoly</trd>
  <trd lng="pl">monopol importu</trd>
  <trd lng="ro">monopolul importurilor</trd>
  <trd lng="ru">&c_i;&c_m;&c_p;&c_o;&c_r;&c_t;&c_n;&c_a;&c_ja;
  <ind>&c_m;&c_o;&c_n;&c_o;&c_p;&c_o;&c_l;&c_i;&c_ja;</ind></trd>
  <trd lng="sk">dovozn&yacute; monopol</trd>
</drv>

<drv mrk="monopo.sxtata0o">
  <kap>&scirc;tata <tld/>o</kap>
  <snc mrk="monopo.sxtata0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <tld lit="M"/>o tenata de la &scirc;tataj organoj.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_ja;&c_r;&c_zh;&c_a;&c_w;&c_n;&c_a;&c_ja;
  <ind>&c_m;&c_a;&c_n;&c_a;&c_p;&c_o;&c_l;&c_ib;&c_ja;</ind></trd>
  <trd lng="de">Staatsmonopol</trd>
  <trd lng="en">government monopoly</trd>
  <trd lng="fr">monopole d'&eacute;tat</trd>
  <trd lng="pl">monopol pa&nacute;stwowy</trd>
  <trd lng="ro">monopol de stat</trd>
  <trd lng="ru">&c_g;&c_o;&c_s;&c_u;&c_d;&c_a;&c_r;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_a;&c_ja;
  <ind>&c_m;&c_o;&c_n;&c_o;&c_p;&c_o;&c_l;&c_i;&c_ja;</ind></trd>
  <trd lng="uk">&c_d;&c_je;&c_r;&c_zh;&c_a;&c_v;&c_n;&c_a; &c_m;&c_o;&c_n;&c_o;&c_p;&c_o;&c_l;&c_ib;&c_ja;</trd>
</drv>

<drv mrk="monopo.0profitoj">
  <kap><tld/>profitoj</kap>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Profitoj, kies kvanto nur eblas pro monopolo.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_a;&c_p;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_ja;
  &c_p;&c_r;&c_y;&c_b;&c_y;&c_t;&c_k;&c_ib;</trd>
    <trdgrp lng="de">
      <trd>Monopolgewinne</trd>,
      <trd>Monopolprofite</trd>
    </trdgrp>
  <trd lng="pl">zyski  <ind>monopolistyczne</ind></trd>
  <trd lng="ro">profiturile monopoliste</trd>
  <trd lng="ru">&c_m;&c_o;&c_n;&c_o;&c_p;&c_o;&c_l;&c_mol;&c_n;&c_a;&c_ja;
  &c_p;&c_r;&c_i;&c_b;&c_y;&c_l;&c_mol;</trd>
  <trdgrp lng="zh">
    <trd>&#22404;&#26029;&#21033;&#28070; <pr>l&#466;ngdu&agrave;nl&igrave;r&ugrave;n</pr></trd>,
    <trd>&#22751;&#26039;&#21033;&#28516; <pr>l&#466;ngdu&agrave;nl&igrave;r&ugrave;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="monopo.0akapitalo">
  <kap><tld/>a kapitalo</kap>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Kapitalo akirita el monopolprofitoj. Ofte: Entrepren(ist)oj kiuj
      akiras monopolprofitojn.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_a;&c_p;&c_o;&c_l;&c_mol;&c_n;&c_y;
  &c_k;&c_a;&c_p;&c_ib;&c_t;&c_a;&c_l;</trd>
  <trd lng="de">Monopolkapital</trd>
  <trd lng="ja">&#29420;&#21344;&#36039;&#26412;  <pr>&#12393;&#12367;&#12379;&#12435;&#12375;&#12411;&#12435;</pr></trd>
  <trd lng="pl">kapita&lstroke;  <ind>monopolistyczny</ind></trd>
  <trd lng="ro">capital de monopol</trd>
  <trd lng="ru">&c_m;&c_o;&c_n;&c_o;&c_p;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j;
  &c_k;&c_a;&c_p;&c_i;&c_t;&c_a;&c_l;</trd>
</drv>
