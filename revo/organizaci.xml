<art mrk="$Id: organizaci.xml,v 1.3 2022/03/24 15:32:03 revo Exp $">
<kap>
  <rad>organizaci</rad>/o
</kap>
<drv mrk="organizaci.0o">
  <kap><tld/>o</kap>
  <snc mrk="organizaci.0o.organizajxo">
    <uzo tip="stl">ARK</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="organi2.0ajxo">Organiza&jcirc;o</ref>:
      <ekz>
        vasta propagando kaj multaj aliaj aferoj povas esti aran&gcirc;ataj nur 
        de internacia <tld/>o
        <fnt>
          <aut>Kabe</aut>,
          <vrk><url ref="https://eo.wikisource.org/wiki/Parolado_de_Kabe_%C4%89e_la_unua_Universala_Kongreso">
          Parolado &ccirc;e la unua Universala Kongreso</url></vrk>,
          <lok>1905</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu konscia esperantisto devas esti membro de
        la oficiala mond<tld/>o U.&#x202F;E.&#x202F;A.
        <fnt>
          <aut>-</aut>,
          <vrk>[reklamo]</vrk>,
          <lok><url ref="http://literaturamondo.org/PDF/1922/LM%201922-03c.pdf">
          Literatura Mondo, 1922:3</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        refondita <tld/>o de zagrebaj esperantistoj
        <fnt>
          <vrk><url ref="https://esperantozagreb.blogspot.com/">Esperanto novosti</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x440;&#x433;&#x430;&#x43D;&#x456;&#x437;&#x430;&#x446;&#x456;&#x44F; (&#x43E;&#x440;&#x433;&#x430;&#x43D;)</trd>,
    <trd>&#x43E;&#x440;&#x433;&#x430;&#x43D;&#x456;&#x437;&#x430;&#x446;&#x456;&#x44F; (&#x434;&#x456;&#x44F;)</trd>
  </trdgrp>
</drv>
</art>

