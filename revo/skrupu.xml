  <rad>skrupul</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="skrupu.0o">
  <kap><tld/>o</kap>
  <snc mrk="skrupu.0o.morala">
    <dif>
      <ref tip="super" cel="dub.0emo">Dubemo</ref> pri la propra &gcirc;usta agado pri aferoj de moralo, de konscienco:
      <ekz>
        li sen ia <tld/>o min faris sia amatino
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi sen ia <tld/>o edzini&gcirc;us kun la sinjoro guberniestro
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la penso liberi&gcirc;i de siaj doloroj <klr>[&#8230;]</klr> venkis &ccirc;iujn restantajn
        <tld/>ojn
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        deziro pla&ccirc;i al tiu aminda eminentulo fine venkis liajn <tld/>ojn, kaj li
        konsentis
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konfli.anim0o">animkonflikto</ref>,
      <ref cel="dub.konscienc0o">konsciencdubo</ref>,
      <ref cel="riproc.konscienc0o">konsciencripro&ccirc;o</ref>,
      <ref cel="sent.kulpo0o">kulposento</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_s;&c_u;&c_m;&c_l;&c_je;&c_n;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_d;&c_b;&c_a;&c_j;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_d;&c_o;&c_b;&c_r;&c_a;&c_s;&c_u;&c_m;&c_l;&c_je;&c_n;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Bedenken</trd>,
      <trd>Skrupel</trd>,
      <trd>Hemmungen</trd>,
      <trd>Vorbehalte</trd>
    </trdgrp>
    <trd lng="nl">gewetensbezwaar</trd>
    <trd lng="pl">skrupu&lstroke;</trd>
    <trdgrp lng="ru">
      <trd>&c_shch;&c_je;&c_p;&c_je;&c_t;&c_i;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_o;&c_v;&c_je;&c_s;&c_t;&c_l;&c_i;&c_v;&c_o;&c_s;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="skrupu.0o.plenuma">
    <dif>
      Pleja zorgemo en plenumo de taskoj:
      <ekz>
        kompetente kaj <tld/>e ju&gcirc;i
        <fnt>
          <bib>LSB</bib>
          <lok>Angkor'a&ubreve;!</lok>
        </fnt>;
      </ekz>
      <ekz>
        la regularo de la societo postulis <tld/>e estetikajn vestadon, konduton kaj
        pensmanieron
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        li subtiris mian palpebron kaj <tld/>e esploris mian okulon
        <fnt>
          <bib>VaK</bib>
          <lok>Dua &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akribi.0o">akribio</ref>,
      <ref cel="asidu.0a">asidueco</ref>,
      <ref cel="preciz.0eco">precizeco</ref>,
      <ref cel="pedant">pedanteco</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_s;&c_k;&c_r;&c_u;&c_p;&c_u;&c_l;&c_jo;&c_z;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_ib;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_u;&c_p;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_t;&c_a;&c_r;&c_a;&c_n;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">Gewissenhaftigkeit</trd>
    <trdgrp lng="hu">
      <trd>lelkiismeretess&eacute;g</trd>,
      <trd>gondoss&aacute;g</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>skrupulatno&sacute;&cacute;</trd>,
      <trd>sumienno&sacute;&cacute;</trd>,
      <trd>dok&lstroke;adno&sacute;&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_k;&c_r;&c_u;&c_p;&c_u;&c_l;&c_jo;&c_z;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_t;&c_shch;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>mor&aacute;ln&iacute; z&aacute;brana</trd>,
    <trd>rozpaky</trd>,
    <trd>skrupule</trd>,
    <trd>&uacute;zkostlivost</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>scrupulousness</trd>,
    <trd>conscientiousness</trd>
  </trdgrp>
  <trd lng="fr">scrupule</trd>
  <trdgrp lng="ja">
    <trd>&#33391;&#24515;&#12398;&#12392;&#12364;&#12417;  [<ind>&#12426;&#12423;&#12358;&#12375;&#12435;&#12398;&#12392;&#12364;&#12417;</ind>]</trd>,
    <trd>&#12383;&#12417;&#12425;&#12356;</trd>,
    <trd>&#30097;&#24565;  [<ind>&#12366;&#12397;&#12435;</ind>]</trd>,
    <trd>&#32048;&#24515;  [<ind>&#12373;&#12356;&#12375;&#12435;</ind>]</trd>,
    <trd>&#32191;&#23494;  [<ind>&#12417;&#12435;&#12415;&#12388;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">scrupule</trd>
  <trdgrp lng="sk">
    <trd>mor&aacute;lna z&aacute;brana</trd>,
    <trd>rozpaky</trd>,
    <trd>&uacute;zkostlivos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_k;&c_r;&c_u;&c_p;&c_u;&c_l;&c_mol;&c_o;&c_z;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_je;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_r;&c_a;&c_n;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_d;&c_a;&c_n;&c_t;&c_i;&c_ch;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_o;&c_v;&c_ib;&c_s;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_u;&c_m;&c_n;&c_ib;&c_v; (&c_u; &c_p;&c_r;&c_a;&c_v;&c_i;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_ib; &c_v;&c_ch;&c_i;&c_n;&c_k;&c_u;</trd>,
    <trd>&c_p;&c_o;&c_v;&c_je;&c_d;&c_ib;&c_n;&c_k;&c_i;)</trd>,
    <trd>&c_d;&c_o;&c_k;&c_ib;&c_r; &c_s;&c_u;&c_m;&c_l;&c_ib;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="skrupu.0a">
  <kap><tld/>a</kap>
  <snc mrk="skrupu.0a.PSI">
    <dif>
      <ref tip="super" cel="zorg.0ema">Zorgema</ref>, ellasanta neniun detalon de surprenita afero:
      <ekz>
        Eduardo rakontis al mi, kun kia <tld/>a delikateco vi respektis la vian
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dektria</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekstreme honesta, <tld/>a, fidinda rigorulo
        <fnt>
          <bib>ChV</bib>
          <lok>23</lok>
        </fnt>;
      </ekz>
      <ekz>
        post <tld/>a ekzamenado, la papero evidentigas sin <klr>[&#8230;]</klr> nedan&gcirc;era
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2013/010928.php"
              >Adori, odori</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konsci1.0a">konscienca</ref>,
      <ref cel="respons.0a.fidinda">responsa <sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_r;&c_u;&c_p;&c_u;&c_l;&c_jo;&c_z;&c_n;&c_y;</trd>,
    <trd>&c_d;&c_b;&c_a;&c_j;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>malichern&yacute;</trd>,
    <trd>pe&ccaron;liv&yacute;</trd>,
    <trd>p&rcaron;esn&yacute;</trd>,
    <trd>&uacute;zkostliv&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>skrupul&ouml;s</trd>,
    <trd>gewissenhaft</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>scrupulous</trd>,
    <trd>conscientious</trd>
  </trdgrp>
  <trd lng="fr">scrupuleux</trd>
  <trdgrp lng="hu">
    <trd>apr&oacute;l&eacute;kos</trd>,
    <trd>lelkiismeretes</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#33391;&#24515;&#30340;&#12394;  [<ind>&#12426;&#12423;&#12358;&#12375;&#12435;&#12390;&#12365;&#12394;</ind>]</trd>,
    <trd>&#27671;&#12398;&#23567;&#12373;&#12356;  [<ind>&#12365;&#12398;&#12385;&#12356;&#12373;&#12356;</ind>]</trd>,
    <trd>&#24910;&#37325;&#12394;  [<ind>&#12375;&#12435;&#12385;&#12423;&#12358;&#12394;</ind>]</trd>,
    <trd>&#32048;&#24515;&#12398;  [<ind>&#12373;&#12356;&#12375;&#12435;&#12398;</ind>]</trd>,
    <trd>&#32191;&#23494;&#12394;  [<ind>&#12417;&#12435;&#12415;&#12388;&#12394;</ind>]</trd>,
    <trd>&#12365;&#12385;&#12423;&#12358;&#12417;&#12435;&#12394;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>skrupulatny</trd>,
    <trd>sumienny</trd>,
    <trd>dok&lstroke;adny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_r;&c_u;&c_p;&c_u;&c_l;&c_jo;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_shch;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>malichern&yacute;</trd>,
    <trd>&uacute;zkostliv&yacute;</trd>,
    <trd>&scaron;krupul&oacute;zny</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_k;&c_r;&c_u;&c_p;&c_u;&c_l;&c_mol;&c_o;&c_z;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_r;&c_je;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_r;&c_a;&c_n;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_je;&c_d;&c_a;&c_n;&c_t;&c_i;&c_ch;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_o;&c_v;&c_ib;&c_s;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="skrupu.0i">
  <kap><tld/>i</kap>
  <snc mrk="skrupu.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="hezit.0i">Heziti</ref>, ne agi pro <tld/>o:
      <ekz>
        mi ja delonge volis diri al vi mian intencon, sed mi samtempe
        <tld/>is pri timigo al vi
        <fnt>
          <aut>Pu Songling, [trad. Guozhu?]</aut>,
          <vrk><url ref="http://www.elerno.cn/bbs">
          Forpelo de Monstro</url></vrk>,
          <lok>2006-07-08</lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;&ccirc;u anka&ubreve; Pitizazo sin fordonas al &ccirc;anta&gcirc;o?&rightquot; &Gcirc;oja demandis, &scirc;ajne sen <tld/>i
        pri interrompado
        <fnt>
          <bib>ChV</bib>
          <lok>31</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_g;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol; &c_u; <ind>&c_n;&c_je;&c_r;&c_a;&c_sh;&c_u;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_ib;</ind></trd>
  </trdgrp>
  <trd lng="de">zaudern</trd>
  <trd lng="fr">se faire un <ind>scrupule</ind> de</trd>
</drv>

<drv mrk="skrupu.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="skrupu.sen0a.PSI">
    <dif>
      Havanta neniun <tld/>on, indiferenta al la sekvoj de siaj agoj:
      <ekz>
        plej ruzaj kaj sen<tld/>aj krimuloj
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        estu singarda, vi havos aferon kun sen<tld/>aj, dan&gcirc;eraj kontra&ubreve;uloj
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        malka&scirc;e sin &jcirc;eti <klr>[&#8230;]</klr> en la batalon kontra&ubreve; la sen<tld/>a hierarkio
        kaj la socia sistemo
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        agu sa&gcirc;e, &ccirc;ar ni havas aferon kun aro da treege sen<tld/>aj virinoj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        nun sen<tld/>aj impostfra&ubreve;duloj ne plu povas esti certaj, ke oni ne malkovros
        iliajn krimojn
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2008/009448.php"
              >Krimaj ri&ccirc;uloj</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hont.sen0a">senhonta</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_d;&c_o;&c_b;&c_r;&c_a;&c_s;&c_u;&c_m;&c_l;&c_je;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_s;&c_a;&c_r;&c_o;&c_m;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_u;&c_m;&c_l;&c_je;&c_n;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bezohledn&yacute;</trd>,
    <trd>nevyb&iacute;rav&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>skrupellos</trd>,
    <trd>gewissenlos</trd>
  </trdgrp>
  <trd lng="en">unscrupulous</trd>
  <trd lng="fr">sans <ind>scrupules</ind></trd>
  <trdgrp lng="hu">
    <trd>agg&aacute;ly</trd>,
    <trd>k&eacute;tely</trd>,
    <trd>lelkiismeretfurdal&aacute;s</trd>,
    <trd>skrupulus</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#24179;&#27671;&#12391;&#24746;&#20107;&#12434;&#12377;&#12427;  [<ind>&#12408;&#12356;&#12365;&#12391;&#12354;&#12367;&#12376;&#12434;&#12377;&#12427;</ind>]</trd>,
    <trd>&#24746;&#36771;&#12394;  [<ind>&#12354;&#12367;&#12425;&#12388;&#12394;</ind>]</trd>
  </trdgrp>
  <trd lng="pl">bez <ind>skrupu&lstroke;&oacute;w</ind></trd>
  <trd lng="ru">&c_b;&c_je;&c_s;&c_s;&c_o;&c_v;&c_je;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">bezoh&lcaron;adn&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_je;&c_z;&c_s;&c_o;&c_r;&c_o;&c_m;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_c;&c_je;&c_r;&c_je;&c_m;&c_o;&c_n;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
