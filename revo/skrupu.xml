<drv mrk="skrupu.0o">
  <kap><tld/>o</kap>
  <snc mrk="skrupu.0o.morala">
    <dif>
      <ref tip="super" cel="dub.0emo">Dubemo</ref> pri la propra &gcirc;usta agado pri aferoj de moralo,
      de konscienco:
      <ekz>
        li sen ia <tld/>o min faris sia amatino
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi sen ia <tld/>o edzini&gcirc;us kun la sinjoro guberniestro
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la penso liberi&gcirc;i de siaj doloroj <klr>[&#8230;]</klr> venkis &ccirc;iujn restantajn
        <tld/>ojn
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        deziro pla&ccirc;i al tiu aminda eminentulo fine venkis liajn <tld/>ojn, kaj li
        konsentis
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konfli.anim0o">animkonflikto</ref>,
      <ref cel="dub.konscienc0o">konsciencdubo</ref>,
      <ref cel="riproc.konscienc0o">konsciencripro&ccirc;o</ref>,
      <ref cel="sent.kulpo0o">kulposento</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_s;&c_u;&c_m;&c_l;&c_je;&c_n;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_d;&c_b;&c_a;&c_j;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_d;&c_o;&c_b;&c_r;&c_a;&c_s;&c_u;&c_m;&c_l;&c_je;&c_n;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Bedenken</trd>,
      <trd>Skrupel</trd>,
      <trd>Hemmungen</trd>,
      <trd>Vorbehalte</trd>
    </trdgrp>
    <trd lng="nl">gewetensbezwaar</trd>
    <trd lng="pl">skrupu&lstroke;</trd>
    <trdgrp lng="ru">
      <trd>&c_shch;&c_je;&c_p;&c_je;&c_t;&c_i;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_o;&c_v;&c_je;&c_s;&c_t;&c_l;&c_i;&c_v;&c_o;&c_s;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="skrupu.0o.plenuma">
    <dif>
      Pleja zorgemo en plenumo de taskoj:
      <ekz>
        kompetente kaj <tld/>e ju&gcirc;i
        <fnt>
          <bib>LSB</bib>
          <lok>Angkor'a&ubreve;!</lok>
        </fnt>;
      </ekz>
      <ekz>
        la regularo de la societo postulis <tld/>e estetikajn vestadon, konduton kaj
        pensmanieron
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        li subtiris mian palpebron kaj <tld/>e esploris mian okulon
        <fnt>
          <bib>VaK</bib>
          <lok>Dua &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akribi.0o">akribio</ref>,
      <ref cel="asidu.0a">asidueco</ref>,
      <ref cel="preciz.0eco">precizeco</ref>,
      <ref cel="pedant">pedanteco</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_s;&c_k;&c_r;&c_u;&c_p;&c_u;&c_l;&c_jo;&c_z;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_ib;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_u;&c_p;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_t;&c_a;&c_r;&c_a;&c_n;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Gewissenhaftigkeit</trd>,
      <trd>Sorgfalt</trd>
    </trdgrp>
    <trdgrp lng="hu">
