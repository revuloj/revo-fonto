<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>skrupul</rad>/o
</kap>
<drv mrk="skrupu.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Maltrankviliga dubo en aferoj de konscienco:
      <ekz>
        pro <tld/>o li rifuzis la donacon;
      </ekz>
      <ekz>
        fari al si <tld/>on pri;
      </ekz>
      <ekz>
        <ind>sen<tld/>a</ind> kanajlo.
        <trdgrp lng="be">
          <trd>&c_n;&c_ja;&c_d;&c_o;&c_b;&c_r;&c_a;&c_s;&c_u;&c_m;&c_l;&c_je;&c_n;&c_n;&c_y;</trd>, 
          <trd>&c_b;&c_je;&c_s;&c_s;&c_a;&c_r;&c_o;&c_m;&c_n;&c_y;</trd>, 
          <trd>&c_n;&c_je;&c_s;&c_u;&c_m;&c_l;&c_je;&c_n;&c_n;&c_y;</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>agg&aacute;ly</trd>,
          <trd>k&eacute;tely</trd>
          <trd>lelkiismeretfurdal&aacute;s</trd>,
          <trd>skrupulus</trd>
        </trdgrp>
        <trd lng="ru">&c_b;&c_je;&c_s;&c_s;&c_o;&c_v;&c_je;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="honest">honesteco</ref>,
      <ref cel="delika">delikateco</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_s;&c_u;&c_m;&c_l;&c_je;&c_n;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>, 
      <trd>&c_d;&c_b;&c_a;&c_j;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>, 
      <trd>&c_d;&c_o;&c_b;&c_r;&c_a;&c_s;&c_u;&c_m;&c_l;&c_je;&c_n;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Skrupel</trd>,
      <trd>Zweifel</trd>,
      <trd>Gewissensnot</trd>,
      <trd>Bedenken</trd>
    </trdgrp>
    <trd lng="nl">gewetensbezwaar</trd>
    <trdgrp lng="ru">
      <trd>&c_shch;&c_je;&c_p;&c_je;&c_t;&c_i;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_o;&c_v;&c_je;&c_s;&c_t;&c_l;&c_i;&c_v;&c_o;&c_s;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Granda akurateco:
      <ekz>
        <ind><tld/>a</ind> esploro<fnt>B</fnt>;
        <trdgrp lng="hu">
          <trd>apr&oacute;l&eacute;kos</trd>,
          <trd>lelkiismeretes</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_s;&c_k;&c_r;&c_u;&c_p;&c_u;&c_l;&c_jo;&c_z;&c_n;&c_y;&c_j;</trd>,
          <trd>&c_t;&c_shch;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <tld/>e traduki &ccirc;iujn nuancojn.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="preciz.0eco">precizeco</ref>,
      <ref cel="pedant">pedanteco</ref>,
      <ref cel="zorg.0o">zorgo</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_s;&c_k;&c_r;&c_u;&c_p;&c_u;&c_l;&c_jo;&c_z;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>, 
      <trd>&c_p;&c_ib;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>, 
      <trd>&c_r;&c_u;&c_p;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>, 
      <trd>&c_s;&c_t;&c_a;&c_r;&c_a;&c_n;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>lelkiismeretess&eacute;g</trd>,
      <trd>gondoss&aacute;g</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_k;&c_r;&c_u;&c_p;&c_u;&c_l;&c_jo;&c_z;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_t;&c_shch;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="de">h&ouml;chste <ind>Gewissenhaftigkeit</ind></trd>
  <trd lng="fr">scrupule</trd>
  <trd lng="nl">scrupule</trd>
</drv>
</art>
