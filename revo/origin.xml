<drv mrk="origin.0e">
  <kap><tld/>e</kap>
   <snc>
    <dif>
     &Ccirc;e la <ref tip="dif" cel="origin.0o">origino</ref>; 
     <ref tip="super" cel="unu.0e">unue</ref>:
     <ekz>
      kontrakto <tld/>e falsita;
     </ekz>
      <ekz>
        <tld/>e Interreto estis precipe usona afero kaj, tute logike, &gcirc;i
        estis anglalingva
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/006881.html">Perkomputila
          
           Tradukado: Universala Retlingvo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la nuna &ccirc;evalo mar&scirc;as per meza fingro de siaj piedoj pro
        adapti&gcirc;o 
        al siaj aktualaj vivkondi&ccirc;oj, <klr>[&#8230;]</klr> <tld/>e ties piedoj
        estis kvarfingraj
        <fnt>
          <bib>Monato</bib>
          <aut>Jean-Yves Santerre</aut>
          <vrk><url ref="&Steloj;monato2/006864.html">Dombestoj:
          &Ccirc;evalrasoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        papo Johano la 23a mem estis <tld/>e mara pirato
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="&Steloj;monato2/007031.html">Katolikismo:
           Johano Hus &#8208; &ccirc;u ne plu herezulo?</url></vrk>
        </fnt>.
      </ekz>
   </dif>
   </snc>
   <trd lng="be">&c_p;&c_je;&c_r;&c_sh;&c_a;&c_p;&c_a;&c_ch;&c_a;&c_t;&c_k;&c_o;&c_v;&c_a;</trd>
  <trd lng="cs">p&uring;vodn&ecaron;</trd>
  <trd lng="de">urspr&uuml;nglich</trd>
  <trd lng="en">originally</trd>
  <trd lng="fr">originellement</trd>
  <trd lng="sk">p&ocirc;vodne</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a; &c_p;&c_o;&c_h;&c_o;&c_d;&c_zh;&c_je;&c_n;&c_n;&c_ja;&c_m;</trd>,
    <trd>&c_v;&c_ib;&c_d; &c_s;&c_a;&c_m;&c_o;&c_g;&c_o; &c_p;&c_o;&c_ch;&c_a;&c_t;&c_k;&c_u;</trd>,
    <trd>&c_v;&c_ib;&c_d; &c_z;&c_a;&c_r;&c_o;&c_d;&c_zh;&c_je;&c_n;&c_n;&c_ja;</trd>
