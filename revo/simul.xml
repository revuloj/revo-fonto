<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: simul.xml,v 1.18 2024/03/24 20:21:57 revo Exp $">
<kap>
  <rad>simul</rad>/i
</kap>

<drv mrk="simul.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="simul.0i.MED">
    <uzo tip="fak">MED</uzo>
    <uzo tip="fak">JUR</uzo>
    <dif>
      Trompe <ref tip="super" cel="sxajn.0igi">&scirc;ajnigi</ref>
      (malsanon, prirabon ktp), imiti simptomojn a&ubreve;
      cirkonstancojn.
    </dif>
    <trd lng="be">&c_s;&c_y;&c_m;&c_u;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="en">
      <trd>feign</trd>,
      <trd>pretend</trd>,
      <trd>dissemble</trd>
    </trdgrp>
    <trdgrp lng="nl">
    <trd>voorwenden</trd>,
    <trd>veinzen</trd>
  </trdgrp>
 <trd lng="ru">&c_s;&c_i;&c_m;&c_u;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="simul.0i.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Studi konduton de sistemo per eksperimentado super konvena
      modelo (<tld/>ilo) programita tiel, ke &gcirc;i ebligas variigi
      parametrojn de la studata fenomeno.
    </dif>
    <trd lng="be">&c_m;&c_a;&c_d;&c_e;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="ca">simular</trd>
    <trd lng="en">simulate</trd>
    <trd lng="es">simular</trd>
    <trd lng="nl">simuleren</trd>
    <trd lng="ru">&c_m;&c_o;&c_d;&c_je;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>napodobit</trd>,
    <trd>napodobovat</trd>,
    <trd>p&rcaron;edst&iacute;rat</trd>,
    <trd>p&rcaron;etva&rcaron;ovat se</trd>,
    <trd>simulovat</trd>
  </trdgrp>
  <trd lng="de">simulieren</trd>
  <trd lng="en">simulate</trd>
  <trd lng="fr">simuler</trd>
  <trd lng="hu">szimul&aacute;l</trd>
  <trdgrp lng="ja">
    <trd>&#x3075;&#x308A;&#x3092;&#x3059;&#x308B;</trd>,
    <trd>&#x30B7;&#x30DF;&#x30E5;&#x30EC;&#x30FC;&#x30C8;&#x3059;&#x308B;</trd>,
    <trd>&#x6A21;&#x64EC;&#x5B9F;&#x9A13;&#x3092;&#x3059;&#x308B;  <pr>&#x3082;&#x304E;&#x3058;&#x3063;&#x3051;&#x3093;&#x3092;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="pl">symulowa&cacute;</trd>
  <trdgrp lng="sk">
    <trd>predstiera&tcaron;</trd>,
    <trd>simulova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&#x43C;&c_u;&#x43B;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_u;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_r;&c_i;&#x43A;&c_i;&c_d;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4EFF;&#x771F; <pr>f&#x1CE;ngzh&#x113;n</pr></trd>,
    <trd>&#x6A21;&#x62DF; <pr>m&#xF3;n&#x1D0;</pr></trd>,
    <trd>&#x6A21;&#x64EC; <pr>m&#xF3;n&#x1D0;</pr></trd>,
    <trd>&#x6A21;&#x4F3C; <pr>m&#xF3;s&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="simul.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="simul.0ado.MED">
    <uzo tip="fak">MED</uzo>
    <uzo tip="fak">JUR</uzo>
    <dif>
      Ago <tld/>i,<sncref ref="simul.0i.MED"/>:
      <ekz>
        <tld/>ado de frenezo.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_y;&c_m;&c_u;&c_l;&c_ja;&c_c;&c_y;&c_ja;</trd>
    <trd lng="nl">voorwendsel</trd>
    <trd lng="ru">&c_s;&c_i;&c_m;&c_u;&c_l;&c_ja;&c_c;&c_i;&c_ja;</trd>
  </snc>
  <snc mrk="simul.0ado.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Ago <tld/>i,<sncref ref="simul.0i.KOMP"/>:
      <ekz>
        trovi akcepteblajn parametrovalorojn per <tld/>ado.
      </ekz>
    </dif>
    <trd lng="be">&c_ib;&c_m;&c_ib;&c_t;&c_a;&c_c;&c_y;&c_j;&c_n;&c_a;&c_je; <ind>&c_m;&c_a;&c_d;&c_e;&c_l;&c_ja;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</ind></trd>
    <trd lng="ca">simulaci&oacute;</trd>
    <trd lng="es">simulaci&oacute;n</trd>
    <trd lng="ru">&c_i;&c_m;&c_i;&c_t;&c_a;&c_c;&c_i;&c_o;&c_n;&c_n;&c_o;&c_je; <ind>&c_m;&c_o;&c_d;&c_je;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</ind></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>p&rcaron;edst&iacute;r&aacute;n&iacute;</trd>,
    <trd>p&rcaron;etv&aacute;&rcaron;ka</trd>,
    <trd>simulace</trd>
  </trdgrp>
  <trd lng="de">Simulieren</trd>
  <trd lng="en">simulation</trd>
  <trd lng="fr">simulation</trd>
  <trd lng="hu">szimul&aacute;l&aacute;s</trd>
  <trdgrp lng="ja">
    <trd>&#x8A50;&#x75C5;  <pr>&#x3055;&#x3073;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x4EEE;&#x75C5;  <pr>&#x3051;&#x3073;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x30B7;&#x30DF;&#x30E5;&#x30EC;&#x30FC;&#x30B7;&#x30E7;&#x30F3;  <pr>&#x3057;&#x307F;&#x3085;&#x308C;&#x30FC;&#x3057;&#x3087;&#x3093;</pr></trd>,
    <trd>&#x6A21;&#x64EC;&#x5B9F;&#x9A13;  <pr>&#x3082;&#x304E;&#x3058;&#x3063;&#x3051;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>simulatie</trd>,
    <trd>simulering</trd>
  </trdgrp>
  <trd lng="pl">symulacja</trd>
  <trdgrp lng="sk">
    <trd>imit&aacute;cia</trd>,
    <trd>modelovanie</trd>,
    <trd>simul&aacute;cia</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&#x43C;&c_u;&#x43B;&#x44F;&c_c;&c_ib;&#x44F;</trd>,
    <trd>&c_u;&c_d;&c_a;&c_v;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_t;&c_v;&#x43E;&c_r;&c_je;&#x43D;&#x43D;&#x44F; &c_u;&#x43C;&#x43E;&c_v; &#x44F;&c_v;&c_i;&c_shch;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4EFF;&#x771F; <pr>f&#x1CE;ngzh&#x113;n</pr></trd>,
    <trd>&#x6A21;&#x62DF; <pr>m&#xF3;n&#x1D0;</pr></trd>,
    <trd>&#x6A21;&#x64EC; <pr>m&#xF3;n&#x1D0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="simul.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Rimedo por <tld/>ado:
    </dif>
    <subsnc mrk="simul.0ilo.TEK">
      <dif>
        Aparato provizanta celatajn kondi&ccirc;ojn dum testado
        a&ubreve; trejnado (ekz-e imitanta sengraviton per senpezeco).
      </dif>
      <trdgrp lng="ru">
        <trd>&c_i;&c_m;&c_i;&c_t;&c_a;&c_c;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j; &c_k;&c_o;&c_m;&c_p;&c_l;&c_je;&c_k;&c_s;</trd>,
        <trd>&c_t;&c_r;&c_je;&c_n;&c_a;&c_zh;&c_jo;&c_r;</trd></trdgrp>
    </subsnc>
    <subsnc mrk="simul.0ilo.KOMP">
      <uzo tip="fak">KOMP</uzo>
      <dif>
        Modelo, precipe komputilo kun programo
        imitanta la strukturon kaj iujn trajtojn
        de studata objekto.
      </dif>
      <trd lng="ca">simulador</trd>
      <trd lng="es">simulador</trd>
      <trd lng="ru">&c_i;&c_m;&c_i;&c_t;&c_a;&c_c;&c_i;&c_o;&c_n;&c_n;&c_a;&c_ja; <ind>&c_m;&c_o;&c_d;&c_je;&c_l;&c_mol;</ind></trd>
    </subsnc>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_m;&c_u;&c_l;&c_ja;&c_t;&c_a;&c_r;</trd>
  <trd lng="cs">simul&aacute;tor</trd>
  <trd lng="de">Simulator</trd>
  <trd lng="en">simulator</trd>
  <trd lng="fr">simulateur</trd>
  <trd lng="hu">szimul&aacute;tor</trd>
  <trdgrp lng="ja">
    <trd>&#x30B7;&#x30DF;&#x30E5;&#x30EC;&#x30FC;&#x30BF;&#x30FC;</trd>,
    <trd>&#x6A21;&#x64EC;&#x8A13;&#x7DF4;&#x88C5;&#x7F6E;  <pr>&#x3082;&#x304E;&#x304F;&#x3093;&#x308C;&#x3093;&#x305D;&#x3046;&#x3061;</pr></trd>
  </trdgrp>
  <trd lng="nl">simulator</trd>
  <trd lng="pl">symulator</trd>
  <trd lng="ru">&c_i;&c_m;&c_i;&c_t;&c_a;&c_t;&c_o;&c_r;</trd>
  <trd lng="sk">simul&aacute;tor</trd>
  <trd lng="uk">&c_s;&c_i;&#x43C;&c_u;&#x43B;&#x44F;&c_t;&#x43E;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#x6A21;&#x62DF;&#x5668; <pr>m&#xF3;n&#x1D0;q&#xEC;</pr></trd>,
    <trd>&#x6A21;&#x64EC;&#x5668; <pr>m&#xF3;n&#x1D0;q&#xEC;</pr></trd>,
    <trd>&#x6A21;&#x62DF;&#x88C5;&#x7F6E; <pr>m&#xF3;n&#x1D0;zhu&#x101;ngzh&#xEC;</pr></trd>,
    <trd>&#x6A21;&#x64EC;&#x88DD;&#x7F6E; <pr>m&#xF3;n&#x1D0;zhu&#x101;ngzh&#xEC;</pr></trd>
  </trdgrp>
</drv>

</art>

