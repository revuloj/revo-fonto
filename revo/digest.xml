<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: digest.xml,v 1.30 2023/11/04 14:29:02 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>digest</rad>/i
</kap>
<drv mrk="digest.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="digest.0i.FIZL">
    <dif>
      Igi nutra&jcirc;on asimilebla, solvante &gcirc;in per specialaj
      sukoj,
      kiujn stomako kaj intestoj sekrecias:
      <ekz>
        enzimo necesa por <tld/>i laktozon<fnt><aut>Krys Ungar</aut>:
        <vrk>Vivu vigle kaj vegetare!</vrk>, <bib>Monato</bib><lok>jaro
        1995a, numero 7a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="asimil.0i.BIO">asimili <sncref/></ref>
    </refgrp>
  </snc>
  <snc mrk="digest.0i.FIG">
   <uzo tip="stl">FIG</uzo>
     <dif>
       Mense akcepti, aprobi novan, nekutiman a&ubreve; &scirc;okan
       ideon:
       <ekz>
         <uzo tip="stl">FIG</uzo>la germanoj devas <tld/>i sian
         historion<fnt><aut>Stefan Maul</aut>:
         <vrk>Germanio-Auschwitz/Malbonaj konsciencoj</vrk>,
         <bib>Monato</bib><lok>jaro 1995a, numero 3a, p. 5a</lok></fnt>.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="asimil.0i.FIG">asimili <sncref/></ref>
     </refgrp>
     <trdgrp lng="ca">
        <trd>pair</trd>,
        <trd>acceptar</trd>
     </trdgrp>
     <trd lng="nl">verwerken</trd>
     <trd lng="pl">przyswaja&cacute;</trd>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_t;&c_r;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
      <trd>pair</trd>,
      <trd>acceptar</trd>
    </trdgrp><trd lng="ca">digerir</trd>
  <trdgrp lng="cs">
    <trd>osvojit si</trd>,
    <trd>tr&aacute;vit</trd>,
    <trd>vst&rcaron;ebat</trd>,
    <trd>za&zcaron;&iacute;vat</trd>
  </trdgrp>
  <trd lng="de">verdauen</trd>
  <trd lng="en">digest</trd>
  <trd lng="es">digerir</trd>
  <trd lng="fr">dig&eacute;rer</trd>
  <trd lng="he">&#x5DC;&#x5B0;&#x5E2;&#x5B7;&#x5DB;&#x5BC;&#x5B5;&#x5DC;</trd>
  <trd lng="hu">em&eacute;szt</trd>
  <trdgrp lng="ja">
    <trd>&#x6D88;&#x5316;&#x3059;&#x308B;  <pr>&#x3057;&#x3087;&#x3046;&#x304B;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x81EA;&#x5206;&#x306E;&#x3082;&#x306E;&#x306B;&#x3059;&#x308B; 
    <pr>&#x3058;&#x3076;&#x3093;&#x306E;&#x3082;&#x306E;&#x306B;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5438;&#x53CE;&#x3059;&#x308B;  <pr>&#x304D;&#x3085;&#x3046;&#x3057;&#x3085;&#x3046;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">verteren</trd><trd lng="nl">verwerken</trd>
  <trd lng="pl">trawi&cacute;</trd><trd lng="pl">przyswaja&cacute;</trd>
  <trdgrp lng="pt">
    <trd>digerir</trd>,
    <trd>fazer <ind>digest&atilde;o</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_v;&c_a;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>digerova&tcaron;</trd>,
    <trd>vyl&uacute;hova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_je;&c_r;&c_je;&c_t;&c_r;&c_a;&c_v;&#x43B;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_je;&c_v;&c_a;&c_r;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_je;&#x43D;. &#x43F;&c_je;&c_r;&c_je;&c_v;&c_a;&c_r;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="digest.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZL</uzo>
  <snc>
    <dif>
      Asimilado de nutra&jcirc;o per la <tld/>aj organoj.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_t;&c_r;&c_a;&c_w;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="ca">digesti&oacute;</trd>
  <trdgrp lng="cs">
    <trd>digesce</trd>,
    <trd>tr&aacute;ven&iacute;</trd>
  </trdgrp>
  <trd lng="de">Verdauung</trd>
  <trdgrp lng="fr">
    <trd>digestion</trd>
  </trdgrp>
  <trd lng="hu">em&eacute;szt&eacute;s</trd>
  <trd lng="ja">&#x6D88;&#x5316;  <pr>&#x3057;&#x3087;&#x3046;&#x304B;</pr></trd>
  <trd lng="nl">vertering</trd>
  <trd lng="pl">trawienie</trd>
  <trd lng="ru">&c_p;&c_i;&c_shch;&c_je;&c_v;&c_a;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">tr&aacute;venie</trd>
  <trd lng="uk">&c_t;&c_r;&c_a;&c_v;&#x43B;&c_je;&#x43D;&#x43D;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x6D88;&#x5316;&#x529B; <pr>xi&#x101;ohu&#xE0;l&#xEC;</pr></trd>,
    <trd>&#x6D88;&#x5316; <pr>xi&#x101;ohu&#xE0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="digest.0iga">
  <kap><tld/>iga</kap>
  <snc>
    <dif>
      Faciliganta la <tld/>on.
    </dif>
  </snc>
  <trd lng="ca">digestiu</trd>
  <trd lng="cs">podporuj&iacute;c&iacute; tr&aacute;ven&iacute;</trd>
  <trd lng="de">verdauungsf&ouml;rdernd</trd>
  <trd lng="fr">digestif<klr tip="ind"> (qui facilite la
  digestion)</klr></trd>
  <trd lng="hu">em&eacute;szt&eacute;st seg&iacute;t&odblac;</trd>
  <trd lng="ja">&#x6D88;&#x5316;&#x3092;&#x4FC3;&#x9032;&#x3059;&#x308B; 
  <pr>&#x3057;&#x3087;&#x3046;&#x304B;&#x3092;&#x305D;&#x304F;&#x3057;&#x3093;&#x3059;&#x308B;</pr></trd>
  <trdgrp lng="nl">
    <trd>digestief</trd>,
    <trd>wat doet <ind>verteren</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_p;&c_o;&c_s;&c_o;&c_b;&c_s;&c_t;&c_v;&c_u;&c_ju;&c_shch;&c_i;&c_j;
  <ind>&c_p;&c_i;&c_shch;&c_je;&c_v;&c_a;&c_r;&c_je;&c_n;&c_i;&c_ju;</ind></trd>
  <trd lng="sk">podporuj&uacute;ci tr&aacute;venie</trd>
  <trd lng="zh">&#x6D88;&#x98DF; <pr>xi&#x101;osh&#xED;</pr></trd>
</drv>

<drv mrk="digest.0ebla">
  <kap><tld/>ebla</kap>
  <uzo tip="fak">FIZL</uzo>
  <snc>
    <dif>
      Tia kiun oni povas <tld/>i.
    </dif>
    <trd lng="ca">digerible</trd>
    <trd lng="de">verdaulich</trd>
    <trd lng="nl">verteerbaar</trd>
    <trd lng="pl">strawny</trd>
  </snc>
  <trd lng="cs">straviteln&yacute;</trd>
  <trd lng="he">&#x5E0;&#x5D9;&#x5EA;&#x5DF; &#x5DC;&#x5E2;&#x5D9;&#x5DB;&#x5D5;&#x5DC;</trd>
  <trd lng="sk">str&aacute;vite&lcaron;n&yacute;</trd>
  <trd lng="uk">&#x43B;&c_je;&c_g;&#x43A;&#x43E;&c_t;&c_r;&c_a;&c_v;&#x43D;&c_i;&c_j;</trd>
</drv>

<drv mrk="digest.ne0ebla">
  <kap>ne<tld/>ebla</kap>
  <uzo tip="fak">FIZL</uzo>
  <snc>
    <dif>
      Tia kiun oni ne povas <tld/>i.
    </dif>
    <trd lng="ca">indigerible</trd>
    <trd lng="de">unverdaulich</trd>
    <trd lng="nl">onverteerbaar</trd>
    <trd lng="pl">niestrawny</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>nestraviteln&yacute;</trd>,
    <trd>nez&aacute;&zcaron;ivn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6D88;&#x5316;&#x3057;&#x306B;&#x304F;&#x3044;  <pr>&#x3057;&#x3087;&#x3046;&#x304B;&#x3057;&#x306B;&#x304F;&#x3044;</pr></trd>,
    <trd>&#x3088;&#x304F;&#x3053;&#x306A;&#x308C;&#x3066;&#x3044;&#x306A;&#x3044;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nestr&aacute;vite&lcaron;n&yacute;</trd>,
    <trd>nez&aacute;&zcaron;ivn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4E0D;&#x80FD;&#x6D88;&#x5316; <pr>b&#xF9;n&#xE9;ngxi&#x101;ohu&#xE0;</pr></trd>,
    <trd>&#x96BE;&#x63A5;&#x53D7; <pr>n&#xE0;nji&#x113;sh&#xF2;u</pr></trd>,
    <trd>&#x65E0;&#x6CD5;&#x6D88;&#x5316; <pr>w&#xFA;f&#x1CE;xi&#x101;ohu&#xE0;</pr></trd>,
    <trd>&#x4E0D;&#x4F1A;&#x6D88;&#x5316;&#x7684; <pr>b&#xF9;hu&#xEC;xi&#x101;ohu&#xE0;de</pr></trd>
  </trdgrp>
</drv>

</art>
