  <trdgrp lng="de">
     <trd>Gummigutt</trd>,
     <trd>Gelbharz</trd></trdgrp>
  <trdgrp lng="hu">
     <trd>m&eacute;zgas&aacute;rga</trd>,
     <trd>gumigutti</trd>
  </trdgrp>
  <trd lng="pt">goma-guta</trd>
  <trd lng="ru">&c_g;&c_u;&c_m;&c_m;&c_i;&c_g;&c_u;&c_t;</trd>
</drv>
