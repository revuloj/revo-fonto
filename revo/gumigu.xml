<art mrk="$Id: gumigu.xml,v 1.17 2023/11/11 11:36:37 revo Exp $">
<kap>
  <rad>gumigut</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="gumigu.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Speco de sekigita flava rezina <ref tip="super" cel="gum.0o">gumo</ref>,
      uzata kiel kolorilo kaj anka&ubreve; kiel laksigilo:
      <ekz>
        sume 12 farboj <klr>[&#x2026;]</klr> iu peco, pli granda &ndash; jen <tld/>o <klr>[&#x2026;]</klr>
        miksu iometon el &gcirc;i, por briligi alian kolora&jcirc;on
        <fnt>
          <vrk><url ref="&CRI;221/2007/11/16/1@72685.htm">Gumiguto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        trikoloraj en ru&gcirc;o, blanko kaj bluo, la malvokoloro, ultramaro, <tld/>o, kaj
        sieno
        <fnt>
          <aut>Edwin A. Abbott, trad. Thomas James</aut>
          <vrk><url ref="http://zz9pza.net/tradukoj/platlando/">Platlando &dash; Romaneto de multaj dimensioj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_m;&c_ib;&c_g;&c_u;&c_t;</trd>
  <trd lng="cs">gumiguta</trd>
  <trdgrp lng="de">
    <trd>Gummigutt</trd>,
    <trd>Gummigutta</trd>,
    <trd>Gelbharz</trd>
  </trdgrp>
  <trd lng="en">gamboge</trd>
  <trdgrp lng="hu">
     <trd>m&eacute;zgas&aacute;rga</trd>,
     <trd>gumigutti</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30AC;&#x30F3;&#x30DC;&#x30FC;&#x30B8;</trd>,
    <trd>&#x85E4;&#x9EC4;  <pr>&#x3068;&#x3046;&#x304A;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="pt">goma-guta</trd>
  <trd lng="ru">&c_g;&c_u;&c_m;&c_m;&c_i;&c_g;&c_u;&c_t;</trd>
  <trd lng="sk">gumiguta</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_u;&c_m;&c_ib;&c_g;&c_u;&c_t;</trd>,
    <trd>&c_k;&c_a;&c_m;&c_je;&c_d;&c_mol;-&c_s;&c_m;&c_o;&c_l;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x85E4;&#x9EC3; <pr>t&#xE9;nghu&#xE1;ng</pr></trd>,
    <trd>&#x7C50;&#x9EC3; <pr>t&#xE9;nghu&#xE1;ng</pr></trd>
  </trdgrp>
</drv>
</art>
