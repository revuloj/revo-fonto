<vortaro>
  <art mrk="$Id: matild.xml,v 1.2 2023/06/27 08:35:45 afido Exp $">
    <kap>
      <rad>Matild</rad>/o
    </kap>

<drv mrk="matild.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">NOM</uzo>
  <snc>
    <dif>
      Virina <ref tip="lst" lst="voko:personaj_nomoj" cel="nom.persona0o.GEN">nomo</ref>
      germandevena:
      <ekz>
        krom la gekastelmastroj kaj <tld/>o, ilia sola filino, trovi&gcirc;is en la salono,
        <klr>[&#8230;]</klr> nur fra&ubreve;lino de Savinako, &scirc;ia intima amikino
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        Eleonoro eniras kurante, <klr>[&#8230;]</klr> etendante bluan paperon al <tld/>o
        <fnt>
          <aut>Baena Nuno</aut>
          <vrk><url
          ref="https://www.litres.ru/book/baena-nuno/ama-stelaro-24176788/chitat-onlayn/page-4/"
              >Ama Stelaro</url></vrk>
          <lok>Sceno kvina</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Matilda</trd>
    <trd lng="en">Matilda</trd>
    <trd lng="nl">Matilde</trd>
    <trd lng="ru">Matilda</trd>
  </snc>
</drv>

  </art>

