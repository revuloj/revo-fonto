<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: distin.xml,v 1.34 2022/09/21 05:42:15 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>disting</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="distin.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="distin.0i.apartigi">
    <dif>
      <ref tip="super" cel="rimark.0i.mense">Rimarki</ref> <ref tip="super"
      cel="difere.0o.KLA">diferenco</ref>n 
      inter objektoj, personoj a&ubreve; ideoj:
      <ekz>
        <tld/>i inter bono kaj malbono
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 19:35</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[oni]</klr> ne povis <tld/>i inter la sonoj de la &gcirc;ojkriado kaj la sonoj de
        la popola plorado
        <fnt>
          <bib>MT</bib>
          <lok>Ezra 3:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni ne povis distingi, kie estis komenco kaj kie estis fino
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i la vo&ccirc;on de kukolo kaj de turdo
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        por <tld/>i ilin unu de la alia, oni nomadis tiun, kiu posedis kvar &ccirc;evalojn, la
        granda Niko, kaj tiun, kiu posedis nur unu &ccirc;evalon, oni nomadis la malgranda
        Niko
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ne povis distingi, &ccirc;u <klr>[la varmego]</klr> estas sekvo de la efektiva
        fajro, &ccirc;u de lia tro granda ardo de amo
        <fnt>
          <bib>Fab1</bib>
          <lok>Persista stana soldato</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[oni a&ubreve;dis]</klr> ian strangan tonon, <klr>[&#8230;]</klr>
        sed pro la bruo de la veturiloj kaj de la movado oni povis <tld/>i &gcirc;in nur
        dum mallonga momento
        <fnt>
          <bib>Fab2</bib>
          <lok>Sonorilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fadeno sur &scirc;ia &scirc;pinilo estis pli delikata ol aranea&jcirc;o, homaj okuloj ne povus
        <tld/>i la apartan fadenon
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Dio de la Izraelidoj <tld/>is vin inter la komunumo de Izrael
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 16:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        la morto ne <tld/>as <klr>(sendiference traktas)</klr>,
        &ccirc;iujn egale atingas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="apart.0igi">apartigi</ref>,
      <ref cel="difere.0igi.KLA">diferencigi<sncref/></ref>,
      <ref cel="kontra8.0i">kontrasti</ref>,
      <ref cel="konfuz.mal0i">malkonfuzi</ref>.
    </refgrp>
    <trd lng="de">unterscheiden</trd>
    <trd lng="fr">distinguer<klr tip="ind"> (reconna&icirc;tre comme
    distinct)</klr></trd>
    <trdgrp lng="pl">
      <trd>odr&oacute;&zdot;nia&cacute;</trd>,
      <trd>rozr&oacute;&zdot;nia&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_l;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_l;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="distin.0i.percepti">
    <dif>
      Klare, precize, nete <ref tip="super" cel="percep.0i">percepti</ref>:
      <ekz>
        regis tia mallumo, ke oni ne multe povis <tld/>i
        <fnt><bib>Fab1</bib>, <lok>Galo&scirc;oj de
        feli&ccirc;o</lok></fnt>;
      </ekz>
      <ekz>
        io saltis en la herbo, sed ili ne povis <tld/>i, kio &gcirc;i estas
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino kaj Rozulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni ne povis <tld/>i la fizionomiojn de la voja&gcirc;antoj
        <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 1</lok></fnt>;
      </ekz>
      <ekz>
        la a&ubreve;gusta &ccirc;ielo estis plena de steloj, <klr>[&#8230;]</klr> oni povis <tld/>i la
        konturojn e&ccirc; de malproksimaj objektoj
        <fnt>
          <bib>IK</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="sens.0i">sensi</ref>,
       <ref cel="vid.0i">vidi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>unterscheiden</trd>;
      <trd>erkennen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>distinguer<klr tip="ind"> (percevoir)</klr></trd>,
      <trd>percevoir</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>rozgranicza&cacute;</trd>,
      <trd>rozeznawa&cacute; si&eogonek;</trd>,
      <trd>wyodr&eogonek;bnia&cacute;</trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_l;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="distin.0i.igi">
    <dif>
      <tld lit="D"/>igi:
      <ekz>
        nobelaj moroj kaj nobelaj manieroj lin <tld/>is
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tiu]</klr> pazigrafio <klr>[&#8230;]</klr> <tld/>as sin per ekstrema simpleco
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>auszeichnen</trd>,
      <trd>abheben <klr>(von etwas)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_t;&c_l;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_r;&c_o;&c_z;&c_mol;&c_n;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_r;&c_a;&c_z;&c_l;&c_i;&c_ch;&c_a;&c_v;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>diferencovat</trd>,
    <trd>odli&scaron;it</trd>,
    <trd>poznat</trd>,
    <trd>rozeznat</trd>,
    <trd>rozli&scaron;ovat</trd>,
    <trd>rozpoznat</trd>
  </trdgrp>
  <trd lng="en">distinguish</trd>
  <trd lng="es">distinguir</trd>
  <trd lng="pt">distinguir</trd>
  <trdgrp lng="sk">
    <trd>rozli&scaron;ova&tcaron;</trd>,
    <trd>rozozna&tcaron;</trd>,
    <trd>rozpozna&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_r;&c_ib;&c_z;&c_n;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_r;&c_ib;&c_z;&c_n;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_p;&c_ib;&c_z;&c_n;&c_a;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_r;&c_ib;&c_z;&c_n;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_z;&c_n;&c_a;&c_ch;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="distin.0o">
  <kap><tld/>o</kap>
  <snc mrk="distin.0o.ago">
    <dif>
      Ago, maniero, fakto <tld/>i:
      <ekz>
        en San-Francisko <klr>[&#8230;]</klr> formi&gcirc;is la <tld/>o inter &leftquot;oficialaj&rightquot; kaj
        &leftquot;laboraj&rightquot; lingvoj
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de la speciaraj karakteroj <klr>[en botaniko]</klr> 
        ravis min <fnt><bib>Revadoj</bib>, <lok>Kvina
        promenado</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[se]</klr> ili elektu la nekuraceblajn el tiuj, kies relativa resani&gcirc;o
        ankora&ubreve; estas esperebla <klr>[&#8230;]</klr> kian metadon <klr>[vi]</klr> opinias
        ta&ubreve;ga por la <tld/>o
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la serba-kroata, ekzemple, estas aparta vorto por la onklo frato de la patro,
        alia por la frato de la patrino, tria por la edzo de la patrina fratino,
        kvara por tiu de la patra fratino <klr>[&#8230;]</klr>, kunvivado en grandaj
        familiaj unuoj necesigis tiujn lingvajn <tld/>ojn
        <fnt>
          <bib>Ret</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="difere.0o.KLA">diferenco<sncref/></ref>,
       <ref cel="kontra8.0o">kontrasto</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Unterscheidung</trd>,
      <trd>Unterschied</trd>,
      <trd>Ausdifferenzierung</trd>
    </trdgrp>
    <trd lng="fr">distinction<klr tip="ind"> (acte)</klr></trd>
    <trdgrp lng="pl">
      <trd>odr&oacute;&zdot;nienie</trd>,
      <trd>rozr&oacute;&zdot;nienie</trd>,
      <trd>rozgraniczenie</trd>,
      <trd>rozeznanie</trd>,
      <trd>wyodr&eogonek;bnienie</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_l;&c_i;&c_ch;&c_i;&c_je;</trd>,
      <trd>&c_o;&c_t;&c_l;&c_i;&c_ch;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc mrk="distin.0o.pro_merito">
    <dif>
      Io, kio rekompencas ies <ref cel="merit.0o">merito</ref>n:
      <ekz>
        kian honoron kaj <tld/>on oni faris pro tio al <nom>Morde&hcirc;aj</nom>
        <fnt>
          <bib>MT</bib>
          <lok>Ester 6:3</lok>
        </fnt>?
      </ekz>
      <ekz>
        tio estas io nekompareble bela kaj la plej alta <tld/>o,
        kiun iam ricevis ia anaso
        <fnt><bib>Fab2</bib>, <lok>Malbela anasido</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="orden.0o.insigno">ordeno<sncref/></ref>,
      <ref cel="premi.0o">premio</ref>
    </refgrp>
    <trd lng="de">Auszeichnung</trd>
    <trd lng="fr">distinction<klr tip="ind">
    (r&eacute;compense)</klr></trd>
    <trdgrp lng="pl">
      <trd>odznaczenie</trd>,
      <trd>dystynkcja</trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_t;&c_l;&c_i;&c_ch;&c_i;&c_je;<klr tip="amb">
    (&c_n;&c_a;&c_g;&c_r;&c_a;&c_d;&c_a;)</klr></trd>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_r;&c_o;&c_z;&c_mol;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>distinkce</trd>,
    <trd>odli&scaron;en&iacute;</trd>,
    <trd>rozezn&aacute;n&iacute;</trd>,
    <trd>uzn&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="es">distinci&oacute;nr</trd>
  <trdgrp lng="he">
    <trd>&he;&bet;&het;&nun;&he;</trd>,
    <trd>&tsadi;&yod;&vav;&fnun; &lamed;&shin;&bet;&het;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odl&iacute;&scaron;enie</trd>,
    <trd>uznanie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_m;&c_ib;&c_n;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_h;&c_o;&c_d;&c_zh;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_z;&c_n;&c_a;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="distin.0a">
  <kap><tld/>a</kap>
  <snc mrk="distin.0a.iga">
    <dif>
      <ref tip="dif" cel="distin.0iga"><tld lit="D"/>iga</ref>:
      <ekz>
        plata nazo kaj grasa dorso estis &gcirc;iaj &gcirc;eneralaj
        <tld/>aj signoj
        <fnt><bib>Fab3</bib>, <lok>Kora sufero</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a principo
        <fnt>
          <bib>LOdE</bib>
          <aut>Sergio Pokrovskij</aut>
          <vrk><url ref="https://tekstaro.com/t?nomo=ondo-de-esperanto">Slavonaj Skriboj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la preparado konsistas el du <tld/>aj laboroj: la amasigo de la
        paroladmaterialo, unue, kaj la forma ordigo de tiu materialo poste.
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        traktitaj estas anka&ubreve; kromakcentoj, <tld/>aj pa&ubreve;zetoj <klr>[&#8230;]</klr>
        kaj elparolo de ne-esperantaj vortoj
        <fnt>
          <bib>Monato</bib>
          <aut>Petro Desmet'</aut>
          <vrk><url ref="&Monato;/2006/008993.php">La unua Zamenhof-monumento de la 21a
          jarcento: dika Pomego!</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="difere.0a">diferenca</ref>,
      <ref cel="divers.0a.malsamspeca">diversa<sncref/></ref>,
      <ref cel="sam.mal0a">malsama</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_a;&c_d;&c_m;&c_je;&c_t;&c_n;&c_y;</trd>,
      <trd>&c_a;&c_s;&c_a;&c_b;&c_l;&c_ib;&c_v;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>heraushebend</trd>,
      <trd>unterscheidend</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>odr&oacute;&zdot;nialny</trd>,
      <trd>rozr&oacute;&zdot;nialny</trd>,
      <trd>rozgraniczalny</trd>,
      <trd>wyodr&eogonek;bnialny</trd>,
      <trd>rozpoznawalny</trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_t;&c_l;&c_i;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="distin.0a.eminenta">
    <dif>
      <tld lit="D"/>i&gcirc;anta per <ref cel="eminen1.0a">eminenta</ref> socia pozicio;
      <ref cel="elegan.0a">eleganta</ref> kaj sindetenema, kiel konvenas al tiaj personoj:
      <ekz>
        aristokrata domo <klr>[&#8230;]</klr> en unu el la malla&ubreve;taj, <tld/>aj kvartaloj
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
        </fnt>;
      </ekz>
      <ekz>
        li estis el <tld/>a deveno: lia patro estis bankiero a&ubreve;
        io tia
        <fnt><bib>UrdHadda</bib>, <lok>p. 5</lok></fnt>;
      </ekz><ekz>
        kial vi ne fumas pipon, Inspektoro?, tio donas
        <ind><tld/>eco</ind>n
        <fnt><bib>UrdHadda</bib>, <lok>p. 14</lok></fnt>.
        <trd lng="fr">distinction<klr tip="ind"> (noblesse)</klr></trd>
      </ekz>
    </dif>
    <ref tip="vid" cel="nobl.0a">nobla</ref>
    <trd lng="be">&c_v;&c_y;&c_b;&c_ib;&c_t;&c_n;&c_y;</trd>
    <trd lng="de">vornehm</trd>
    <trd lng="fr">distingu&eacute;<klr tip="ind"> (noble)</klr></trd>
    <trdgrp lng="pl">
      <trd>dystyngowany</trd>,
      <trd>doborowy</trd>,
      <trd>wybitny</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_v;&c_y;&c_d;&c_a;&c_ju;&c_shch;&c_i;&c_j;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>distink&ccaron;n&iacute;</trd>,
    <trd>odli&scaron;ovac&iacute;</trd>,
    <trd>rozli&scaron;ovac&iacute;</trd>
  </trdgrp>
  <trd lng="he">&mem;&vav;&bet;&het;&fnun;</trd>
  <trdgrp lng="sk">
    <trd>u&scaron;&lcaron;achtil&yacute;</trd>,
    <trd>vyberan&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_m;&c_ib;&c_t;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_h;&c_a;&c_r;&c_a;&c_k;&c_t;&c_je;&c_r;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_p;&c_je;&c_c;&c_i;&c_f;&c_ib;&c_ch;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="distin.0ebla">
  <kap><tld/>ebla</kap>
  <snc mrk="distin.0ebla.KOMUNE">
    <dif>
      Kiu sufi&ccirc;e netas, sufi&ccirc;e kontrastas kun &ccirc;irka&ubreve;a&jcirc;o,
      por ke oni &gcirc;in <tld/>u:
      <ekz>
        la luno eklevi&gcirc;is, kaj iliaj viza&gcirc;oj estis <tld/>eblaj
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        siblanta apena&ubreve; <ind><tld/>ebla vo&ccirc;o</ind>
        <fnt>
          <bib>ChL</bib>
        </fnt>;
        <trd lng="de"><ind>verst&auml;ndlich</ind>e Stimme</trd>
      </ekz>
      <ekz>
        la blankete-grizaj piramidoj estis <tld/>eblaj kiel
        siluetoj en la klara aero en la flanko de la dezerto
        <fnt><bib>Fab4</bib>, <lok>Filino de la mar&ccirc;a
        re&gcirc;o</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kompre.0ebla">komprenebla</ref>,
      <ref cel="vid.0ebla">videbla</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>unterscheidbar</trd>,
      <trd>erkennbar</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_m;&c_je;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_a;&c_w;&c_v;&c_a;&c_zh;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>distinct</trd>,
    <trd>distinguable</trd>
  </trdgrp>
</drv>

<drv mrk="distin.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Konigi ion a&ubreve; iun kiel diferencan de alia a&ubreve;
      eminentan inter la aliaj:
      <ekz>
        lia &ccirc;apo <tld/>igis lin inter &ccirc;iuj liaj kamaradoj;
      </ekz>
      <ekz>
        inter &ccirc;iuj &ccirc;i eblaj enhavoj trovi&gcirc;as trajtoj kiuj kunligas pli
        firme parton el ili kaj kiuj <tld/>igas ilin de la aliaj
        <fnt>
          <aut>I. Lapenna</aut>: <vrk>Retoriko</vrk>.
          <lok>&Ccirc;ap. 13ª.</lok>
        </fnt>;
       </ekz>
      <ekz>
        <klr>[lin]</klr> <tld/>igas atenta realisma observokapablo
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
         kiel nomi&gcirc;as speciala signo metita sur io kaj servanta por
         rekonigi kaj <tld/>igi &gcirc;in
        <fnt>
          <aut>William Auld</aut>:
          <vrk>Pa&scirc;oj al plena posedo</vrk>.
          <lok><url
          ref="http://www.elerno.cn/elibro/pasoj%20al%20plena%20posedo/deksesa.htm">Leciono
          16ª.</url></lok>
        </fnt>?
     </ekz> 
    </dif>
    <refgrp tip="vid">
      <ref cel="apart.0igi">apartigi</ref>,
      <ref cel="difere.0igi">diferencigi</ref>,
      <ref cel="karakt1.0i">karakterizi</ref>,
      <ref cel="kontra8.0igi">kontrastigi</ref>,
      <ref cel="mark.0i">marki</ref>,
      <ref cel="sign.0i">signi</ref>
    </refgrp>
    <trd lng="de">auszeichnen</trd>
    <trdgrp lng="fr">
      <trd>distinguer<klr tip="ind"> (caract&eacute;riser)</klr></trd>,
      <trd>caract&eacute;riser</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>odznacza&cacute;</trd>,
      <trd>wyr&oacute;&zdot;nia&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_t;&c_l;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_d;&c_je;&c_l;&c_ja;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_r;&c_o;&c_z;&c_mol;&c_n;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_l;&c_u;&c_ch;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">auszeichnen</trd>
  <trd lng="he">&lamed;&he;&bet;&het;&yod;&fnun;</trd>
</drv>

<drv mrk="distin.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Sin <ref cel="montr.0igxi">montri</ref> diferenca de aliaj:
      <ekz>
        la esenco de &ccirc;iuj religioj estas la sama, kaj ili <tld/>i&gcirc;as unu de alia nur per
        legendoj kaj moroj
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Dogmoj de Hilelismo</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">sich <ind>unterscheiden</ind></trd>
    <trd lng="fr">se <ind>distinguer</ind><klr tip="ind"> (se
    diff&eacute;rencier)</klr></trd>
    <trdgrp lng="pl">
      <trd>odznacza&cacute; si&eogonek;</trd>,
      <trd>wyr&oacute;&zdot;nia&cacute; si&eogonek;</trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_t;&c_l;&c_i;&c_ch;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
  <snc>
    <dif>
      Sin montri diferenca de alia per ia supereco, merito; esti supera,
      eminenta:
      <ekz>
        en la &ccirc;ambro staris multe da belaj mebloj, inter kiuj <tld/>i&gcirc;is <klr>[&#8230;]</klr>
        tre eleganta kaj multekosta fortepiano
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tiu]</klr> kreska&jcirc;o <klr>[&#8230;]</klr> <tld/>i&gcirc;as per kreskado tiel rapida, ke
        en la da&ubreve;ro de tri &gcirc;is kvar monatoj &gcirc;i atingas la altecon de 2 metroj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        perfekta kavaliro, kun la plej bonegaj <tld/>i&gcirc;oj
        <fnt>
          <bib>Hamlet</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bril.0i">brili</ref>,
      <ref cel="kontra8">kontrasti</ref>,
      <ref cel="relief">elreliefigi</ref>.
    </refgrp>
    <trd lng="de">sich <ind>auszeichnen</ind></trd>
    <trdgrp lng="fr">
      <trd>se <ind>distinguer</ind><klr tip="ind">
      (s'illustrer)</klr></trd>,
      <trd>se <ind>signaler</ind><klr tip="ind"> (se
      distinguer)</klr></trd>,
      <trd>s'<ind>illustrer</ind><klr tip="ind"> (se
      distinguer)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>odznacza&cacute; si&eogonek;</trd>,
      <trd>wyr&oacute;&zdot;nia&cacute; si&eogonek;</trd>,
      <trd>wybija&cacute; si&eogonek;</trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_t;&c_l;&c_i;&c_ch;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
  <snc>
    <dif>
      Sin montri klare kaj precize:
      <ekz>
        originala mantelo kun punca kapu&ccirc;o, akre <tld/>i&gcirc;anta &ccirc;e la profunda nigreco de
        la haroj kaj malheleco de la viza&gcirc;o
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">sich <ind>abzeichnen</ind></trd>
    <trdgrp lng="fr">
      <trd>se <ind>distinguer</ind><klr tip="ind">
      (appara&icirc;tre)</klr></trd>,
      <trd>appara&icirc;tre</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>odznacza&cacute; si&eogonek;</trd>,
      <trd>wyr&oacute;&zdot;nia&cacute; si&eogonek;</trd>.
      <trd>odcina&cacute; si&eogonek;</trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_l;&c_i;&c_ch;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_r;&c_o;&c_z;&c_mol;&c_n;&c_ib;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_l;&c_u;&c_ch;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="he">&lamed;&he;&tsadi;&tet;&yod;&yod;&fnun;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_r;&c_ib;&c_z;&c_n;&c_ja;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_r;&c_ib;&c_z;&c_n;&c_ja;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="distin.0iga">
  <kap><tld/>iga</kap>
  <snc>
    <dif>
      Klare, nete diferenciganta; kiu precize karakterizas:
      <ekz>
        la <tld/>iga eco de bona komedio estas, ke &gcirc;i ridigas;
      </ekz><ekz>
        se oni volas uzi &ccirc;i tian vorton en &gcirc;ia logika signifo, oni
        eble elparolu kun <tld/>iga kromakcento je la prefikso &#8218;re&#8216;
        <fnt><bib>PMEG</bib>,
          <lok><url ref="&PMEG;vortfarado/afiksoj/prefiksoj/re.html">Nelogikaj
          RE-vortoj</url></lok></fnt>.
      </ekz>
    </dif>
    <trd lng="fr">distinctif</trd>
    <trdgrp lng="pl">
      <trd>odr&oacute;&zdot;nialny</trd>,
      <trd>rozr&oacute;&zdot;nialny</trd>,
      <trd>rozgraniczalny</trd>,
      <trd>wyodr&eogonek;bnialny</trd>,
      <trd>rozpoznawalny</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_t;&c_l;&c_i;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_o;&c_t;&c_l;&c_i;&c_ch;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_m;&c_je;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_s;&c_a;&c_b;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_h;&c_a;&c_r;&c_a;&c_k;&c_t;&c_e;&c_r;&c_n;&c_y;</trd>
  </trdgrp>
<trd lng="de">besonders</trd>
</drv>

<drv mrk="distin.0igxa">
  <kap><tld/>i&gcirc;a</kap>
  <snc>
    <dif>
      Sin montranta diferenca de ceteraj:
      <ekz>
        numeraj adresoj kaj ret-identigiloj nepre estu asignitaj la&ubreve; <tld/>i&gcirc;a kaj
        konsekvenca maniero
        <fnt>
          <bib>MD11</bib>
          <vrk>Mastroj de Interreto</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">distinctif</trd>
    <trdgrp lng="pl">
      <trd>odr&oacute;&zdot;niaj&aogonek;cy si&eogonek;</trd>,
      <trd>odcinaj&aogonek;cy si&eogonek;</trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_t;&c_l;&c_i;&c_ch;&c_a;&c_ju;&c_shch;&c_i;&c_j;&c_s;&c_ja;</trd>
  </snc>
  <snc>
    <dif>
      Sin montranta supera, eminenta, bonega:
      <ekz>
        la dua <tld/>i&gcirc;a eco de lingvo arta estas &gcirc;ia perfekteco
        <fnt>
          <bib>EE</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="star.el0a">elstara</ref>,
      <ref cel="relief">reliefa</ref>,
      <ref cel="vulgar">malvulgara</ref>.
    </refgrp>
    <trdgrp lng="pl">
      <trd>odznaczaj&aogonek;cy si&eogonek;</trd>,
      <trd>wyr&oacute;&zdot;niaj&aogonek;cy si&eogonek;</trd>.
      <trd>odcina&cacute;aj&aogonek;cy si&eogonek;</trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_t;&c_l;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_m;&c_je;&c_t;&c_n;&c_y;</trd>
</drv>

<drv mrk="distin.0inda">
  <kap><tld/>inda</kap>
  <snc>
    <dif>
      Meritanta pro ia supereco, ke oni <tld/>u lin:
      <ekz>
        mia amato estas <klr>[&#8230;]</klr> <tld/>inda inter dekmilo
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 5:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi vizitis la plej <tld/>indajn domojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        vigla societo, <tld/>inda societo lin &ccirc;irka&ubreve;is
        <fnt><bib>Fab3</bib>, <lok>Nokta &ccirc;apo de
        fra&ubreve;lo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_d;&c_a;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_v;&c_y;&c_d;&c_a;&c_t;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>vynikaj&iacute;c&iacute;</trd>,
    <trd>v&yacute;zna&ccaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>vornehm</trd>,
    <trd>vorz&uuml;glich</trd>,
    <trd>wichtig</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;minent</trd>,
    <trd>distingu&eacute;<klr tip="ind"> (&eacute;minent)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_y;&c_d;&c_a;&c_ju;&c_shch;&c_i;&c_j;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">v&yacute;zna&ccaron;n&yacute;</trd>
</drv>

<drv mrk="distin.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      <ref tip="super" cel="sign.0o.KOMUNE">Signo</ref>, per kiu oni povas <tld/>i:
      <ekz>
        vidinte &scirc;afon kaj a&ubreve;dinte &gcirc;ian blekadon, la homo retenis precize la blekadon
        kiel la plej karakterizan <tld/>ilon
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        la lingvokono estas la sola komuna <tld/>ilo de la anoj
        <fnt>
          <bib>LOdE</bib>
          <aut>Giorgio Silfer</aut>
          <vrk><url ref="https://tekstaro.com/t?nomo=ondo-de-esperanto">La Kvintezo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        dekoraciita de tiom da e&ubreve;ropaj premioj kiom bre&jcirc;neva mar&scirc;alo ricevis da
        <tld/>iloj
        <fnt>
          <bib>MD11</bib>
          <vrk>De solidareco al supereco </vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="difere.0igilo.KLA">diferencigilo</ref>,
      <ref cel="ec.0o">eco</ref>,
      <ref cel="karakt1.0o.KLA">karakterizo</ref>,
      <ref cel="trajt.0o">trajto</ref>
    </refgrp>
    <refgrp tip="vid">
       <ref cel="orden.0o.insigno">ordeno<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_m;&c_je;&c_t;&c_n;&c_a;&c_ja; &c_r;&c_y;&c_s;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>charakteristick&yacute; znak</trd>,
    <trd>pozn&aacute;vac&iacute; znamen&iacute;</trd>,
    <trd>rozli&scaron;ovac&iacute; znak</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Merkmal</trd>,
    <trd>Unterscheidungsmerkmal</trd>,
    <trd>Auszeichnung</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>identifying feature</trd>,
    <trd>signature</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>caract&eacute;ristique</trd>,
    <trd>signe<klr tip="amb"> (distinctif)</klr></trd></trdgrp>
  <trdgrp lng="pl">
    <trd>wyr&oacute;&zdot;nik</trd>,
    <trd>odznaczenie</trd>,
    <trd>odznaka</trd>,
    <trd>znami&eogonek;<klr> (cecha szczeg&oacute;lna)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_l;&c_i;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;
    &c_p;&c_r;&c_i;&c_z;&c_n;&c_a;&c_k;</trd>,
    <trd>&c_s;&c_p;&c_je;&c_c;&c_i;&c_f;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;
    &c_p;&c_r;&c_i;&c_z;&c_n;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pozn&aacute;vacie znamenie</trd>,
    <trd>rozli&scaron;ovac&iacute; znak</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_m;&c_ib;&c_t;&c_n;&c_a;</trd>,
    <trd>&c_s;&c_p;&c_je;&c_c;&c_i;&c_f;&c_ib;&c_ch;&c_n;&c_a; &c_o;&c_z;&c_n;&c_a;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="distin.0ito">
  <kap><tld/>ito

<!-- ne vidataj:,
    <var><kap><tld/>ulo</kap></var>,
    <var><kap><tld/>i&gcirc;ulo</kap></var>,
    <var><kap><tld/>indulo</kap></var>
-->
  </kap>
  <snc>
    <dif>
      Malvulgara, eminenta homo:
      <ekz>
        tio estas la <tld/>itoj el la komunumo,
        la estroj de la triboj
        <fnt>
          <bib>MT</bib>,
          <lok>&Nom;, 16:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        David kunvenigis en Jerusalemon &ccirc;iujn estrojn de Izrael, <klr>[&#8230;]</klr> heroojn,
        kaj &ccirc;iujn <tld/>itojn
        <fnt>
          <bib>MT</bib>
          <lok>I. Kroniko 28:1</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="grav.0a">grav</ref>ulo,
      <ref cel="elit.0o">elito</ref>,
      <ref cel="krem.0o">kremo</ref>
    </refgrp>
    <trdgrp lng="pl">
      <trd><klr>cz&lstroke;owiek </klr>dystyngowany</trd>,
      <trd><klr>cz&lstroke;owiek </klr>odznaczony</trd>,
      <trd><klr>cz&lstroke;owiek </klr>wyr&oacute;&zdot;niaj&aogonek;cy
      si&eogonek;</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_y;&c_d;&c_a;&c_ju;&c_shch;&c_i;&c_j;&c_s;&c_ja;
    &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_d;&c_a;&c_t;&c_n;&c_y; &c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;</trd>
<trd lng="de">Ausgezeichneter</trd>
</drv>

<drv mrk="distin.ne0ebla">
  <kap>ne<tld/>ebla</kap>
  <snc mrk="distin.ne0ebla.KOMUNE">
    <dif>
      Ne <ref tip="ant" cel="net.0a.klara">neta</ref>, tro malproksima, simila al &ccirc;irka&ubreve;aj a&jcirc;oj,
      tiel ke oni &gcirc;in ne <tld/>as; <ref cel="nebul.0a.FIG">nebula</ref>:
      <ekz>
        lingvo malklara kaj ne<tld/>ebla
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 33:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        la skribo ofte aperas tiel pala, tiel ne<tld/>ebla,
        ke oni pensas, ke oni havas anta&ubreve; si malplenan folion
        <fnt><bib>Fab3</bib>, <lok>&Scirc;tono de la
        sa&gcirc;uloj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_r;&c_a;&c_s;&c_p;&c_a;&c_z;&c_n;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_ja;&c_s;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>unkenntlich</trd>,
    <trd>verwaschen</trd>,
  </trdgrp>
  <trd lng="en">indistinguishable</trd>
  <trd lng="fr">indistinct</trd>
</drv>

