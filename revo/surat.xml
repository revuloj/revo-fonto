<vortaro>
  <art mrk="$Id: surat.xml,v 1.2 2022/05/05 14:43:49 afido Exp $">
    <kap>
      <rad>Surat</rad>/o
    </kap>

<drv mrk="surat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="surat.0o.urbo">
    <dif>
      Granda <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      en la okcidento de <ref tip="malprt" cel="barat.B0o">Barato</ref>:
      <ekz>
        la Marata re&gcirc;o <nom>&Scirc;iba&gcirc;i</nom> <klr>[&#8230;]</klr> kolektis multajn tezorojn, forrabinte la
        ri&ccirc;an &scirc;taton de <tld/>o
        <fnt>
          <aut>Jwaharlal Nehru, trad. Syam Sundar Pal</aut>
          <vrk><url ref="http://koki.o.oo7.jp/Leteroj_de_Patro_al_sia_Filino.htm"
          >Leteroj de Patro al sia Filino &dash; La Marata Imperio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        estis en la hinda urbo <tld/>o kafejo <klr>[&#8230;]</klr>
        <fnt>
          <aut>Lev Tolstoj</aut>
          <vrk><url ref="&LOdE;counter/counter.php?libro=12">Dio - unu por &ccirc;iuj - La Ondo de Esperanto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Surat</trd>
    <trd lng="en">Surat</trd>
    <trd lng="fr">Surate</trd>
    <trd lng="gu">&#2744;&#2753;&#2736;&#2724;</trd>
    <trd lng="hi">&#2360;&#2369;&#2352;&#2340;</trd>
  </snc>
</drv>

