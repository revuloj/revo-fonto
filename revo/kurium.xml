  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 96, atompezo 247,0703, simbolo Cm. <ref tip="super"
      cel="urani.trans0o">Transuranio</ref>. &Ccirc;iuj izotopoj de tiu
elemento
      estas radioaktivaj.
    </dif>
  </snc>
  <trd lng="de">Curium</trd>
  <trd lng="hu">k&udblac;rium</trd>
  <trd lng="nl">curium</trd>
  <trd lng="ru">&c_k;&c_ju;&c_r;&c_i;&c_j;</trd>
</drv>
</art>
