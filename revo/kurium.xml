<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kurium</rad>/o
</kap>
<drv mrk="kurium.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref>,
      <ref tip="super" cel="urani.trans0o">transuranio</ref> kun
      atomnumero 96, atompezo 247,0703, simbolo Cm.
      &Ccirc;iuj izotopoj de tiu elemento estas radioaktivaj.
    </dif>
  </snc>                            
  <trd lng="be">&c_k;&c_ju;&c_r;&c_y;&c_j;</trd>
  <trd lng="br">kuriom</trd>
  <trd lng="de">Curium</trd>
  <trd lng="en">curium</trd>
  <trd lng="es">curio</trd>
  <trd lng="fr">curium</trd>
  <trd lng="hu">k&udblac;rium</trd>
  <trd lng="it">curio</trd>
  <trd lng="nl">curium</trd>
  <trd lng="pt">c&uacute;rio</trd>
  <trd lng="ru">&c_k;&c_ju;&c_r;&c_i;&c_j;</trd>
</drv>
</art>
