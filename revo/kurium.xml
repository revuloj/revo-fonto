<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kurium.xml,v 1.21 2023/12/03 09:59:04 revo Exp $">
<kap>
  <rad>kurium</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kurium.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <mlg kod="IUPAC">Cm</mlg>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="96">elemento</ref>,
      <ref tip="super" cel="urani.trans0o">transuranio</ref> kun
      atomnumero 96, atompezo 247,0703;
      kies &ccirc;iuj <ref tip="super" cel="izotop.0o">izotopoj</ref> 
      estas radioaktivaj, aktinoido:
      <ekz>
        <tld/>o estas ar&gcirc;ent-kolora posturania radioaktiva metala elemento kiun 
        oni povas fari bombardante plutonion per alfaradioj, &gcirc;i estas nomata 
        la&ubreve; <nom>Marie</nom> kaj <nom>Pierre Curie</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=28865">Kuriumo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ju;&c_r;&c_y;&c_j;</trd>
  <trd lng="br">kuriom</trd>
  <trd lng="cs">curium</trd>
  <trd lng="de">Curium</trd>
  <trd lng="en">curium</trd>
  <trd lng="es">curio</trd>
  <trd lng="fa">&f_k;&a_w;&a_r;&f_y;&a_m;</trd>
  <trd lng="fr">curium</trd>
  <trd lng="hu">k&udblac;rium</trd>
  <trd lng="id">kurium</trd>
  <trd lng="it">curio</trd>
  <trd lng="la">Curium</trd>
  <trd lng="nl">curium</trd>
  <trd lng="pl">kiur</trd>
  <trd lng="pt">c&uacute;rio</trd>
  <trd lng="ru">&c_k;&c_ju;&c_r;&c_i;&c_j;</trd>
  <trd lng="sk">curium</trd>
  <trd lng="sv">curium</trd>
  <trd lng="uk">&#x43A;&#x44E;&#x440;&#x456;&#x439;</trd>
  <trdgrp lng="zh">
    <trd>&#x92E6; <pr>j&#xFA;</pr></trd>,
    <trd>&#x9514; <pr>j&#xFA;</pr></trd>
  </trdgrp>
</drv>
</art>
