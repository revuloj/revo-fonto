<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: steb.xml,v 1.25 2024/05/02 14:24:48 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>steb</rad>/i <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="steb.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="steb.0i.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Kun<ref tip="super" cel="kudr.0i">kudri</ref> plurajn &scirc;tofpecojn per seninterrompa kudro:
      <ekz>
        blanka <tld/>ita litkovrilo
        <klr>(kvadrate kunkudrita por ke la plumoj ne migru)</klr>
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>steppen</trd>,
      <trd>absteppen</trd>,
      <trd>durchn&auml;hen</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x7D29; <pr>zh&#xEC;</pr></trd>,
    <trd>&#x7D40; <pr>ch&#xF9;</pr></trd>,
    <trd>&#x7ECC; <pr>ch&#xF9;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_y;&c_c;&c_mol; <klr>(&c_n;&c_a; &c_sh;&c_v;&c_je;&c_j;&c_n;&c_a;&c_j; &c_m;&c_a;&c_sh;&c_y;&c_n;&c_c;&c_y;)</klr></trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">cosir a m&agrave;quina</trd>
  <trdgrp lng="cs">
    <trd>pro&scaron;&iacute;vat</trd>,
    <trd>&scaron;tepovat</trd>,
    <trd>&scaron;&iacute;t na stroji</trd>
  </trdgrp>
  <trd lng="de"><ofc>*</ofc>steppen</trd>
  <trdgrp lng="en">
    <trd><ofc>*</ofc>quilt</trd>,
    <trd><ofc>*</ofc>stitch <klr>(back)</klr></trd>
  </trdgrp>
  <trd lng="fa">&a_d;&a_w;&a_kh;&a_t;&a_n;<klr tip="amb"> (&a_b;&a_A; &f_ch;&a_r;&a_kh;
  &a_kh;&f_y;&a_A;&a_T;&f_y;)</klr></trd>
  <trd lng="fr"><ofc>*</ofc>piquer <klr>(couture)</klr></trd>
  <trd lng="hu">varr<klr tip="ind"> (g&eacute;ppel)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x30DF;&#x30B7;&#x30F3;&#12391;&#x7E2B;&#12358;  <pr>&#x307F;&#12375;&#12435;&#12391;&#x306C;&#12358;</pr></trd>,
    <trd>&#21512;&#x308F;&#x305B;&#x7E2B;&#12356;&#12377;&#x308B;  <pr>&#12354;&#x308F;&#x305B;&#x306C;&#12356;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">stikken<klr tip="ind"> (m.naaimachine)</klr></trd>
  <trdgrp lng="pl">
    <trd><ofc>*</ofc>stebnowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_i;&c_t;&c_mol; &c_n;&c_a; &c_sh;&c_v;&c_je;&c_j;&c_n;&c_o;&c_j; &c_m;&c_a;&c_sh;&c_i;&c_n;&c_k;&c_je;</trd>,
    <trd><ofc>*</ofc>&c_s;&c_t;&c_r;&c_o;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pre&scaron;&iacute;va&tcaron; <klr>(papl&oacute;n, vestu)</klr></trd>,
    <trd>pre&scaron;&iacute;va&tcaron; <klr>(papl&oacute;n| vestu)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_sh;&c_i;&c_t;&c_i; &c_n;&c_a; &c_m;&c_a;&c_sh;&c_i;&c_n;&c_c;&c_ib;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_o;&c_ch;&c_i;&c_t;&c_i; <klr>(&c_sh;&c_i;&c_t;&c_mol;)</klr></trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_mol;&c_o;&c_b;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="steb.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Kudro a&ubreve; kudrero farata dum <tld/>ado:
      <ekz>
        <klr>[li]</klr> da&ubreve;rigis la kudradon kaj pro &gcirc;ojo faris pli kaj pli grandajn
        <tld/>ojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>La kura&gcirc;a tajloreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ankora&ubreve; kelkaj <tld/>oj kaj fine la blankaj dentoj tratran&ccirc;as <klr>[&#8230;]</klr> la
        fadenon
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        per lar&gcirc;aj kruco<tld/>oj li riparis la manikon
        <fnt><aut>J. Baghy</aut>, <vrk>Sur Sanga Tero</vrk>,
        <lok>1933</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_v;&c_o;&c_k;</trd>
  <trdgrp lng="de">
    <trd>Steppnaht</trd>,
    <trd>Steppstich</trd>
  </trdgrp>
</drv>

<drv mrk="steb.0ilo">
  <kap><tld/>ilo <fnt><bib>JED</bib></fnt></kap>
  <snc mrk="steb.0ilo.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="masxin.kudro0o.TEKS">Kudroma&scirc;ino</ref> per kiu oni <tld/>as, kutime
      kunplektanta du fadenojn al
      seninterrompa kudro.
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_v;&c_je;&c_j;&c_n;&c_a;&c_ja; &c_m;&c_a;&c_sh;&c_y;&c_n;&c_k;&c_a;</trd>
  <trd lng="ca">m&agrave;quina de cosir</trd>
  <trd lng="cs">jehla do &scaron;ic&iacute;ho stroje</trd>
    <trdgrp lng="de">
      <trd>Steppmaschine</trd>
    </trdgrp>
  <trdgrp lng="en">
    <trd>quilting machine</trd>,
    <trd>stitching machine</trd>
  </trdgrp>
  <trd lng="fa">&f_ch;&a_r;&a_kh; &a_kh;&f_y;&a_A;&a_T;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>machine &agrave; piquer</trd>,
    <trd>piqueuse</trd>
  </trdgrp>
  <trd lng="hu">varr&oacute;g&eacute;p</trd>
  <trd lng="ja">&#x30DF;&#x30B7;&#x30F3;&#x91DD; 
  <pr>&#x30DF;&#x30B7;&#x30F3;&#x306F;&#x308A;</pr></trd>
  <trd lng="ru">&c_sh;&c_v;&c_je;&c_j;&c_n;&c_a;&c_ja; &c_m;&c_a;&c_sh;&c_i;&c_n;&c_k;&c_a;</trd>
  <trd lng="sk">ihla do &scaron;ijacieho stroja</trd>
  <trd lng="uk">&c_g;&c_o;&c_l;&c_k;&c_a; (&c_d;&c_l;&c_ja; &c_sh;&c_v;&c_je;&c_j;&c_n;&c_o;&c_ji;
  &c_m;&c_a;&c_sh;&c_i;&c_n;&c_i;)</trd>
</drv>
</art>
