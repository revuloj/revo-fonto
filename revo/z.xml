<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc><rad>zo</rad>  <fnt><bib>F</bib>,
      <lok>Ekzercaro</lok></fnt>
    </kap>
<drv mrk="z.0">
  <kap><tld/></kap>
  <uzo tip="fak">LIN</uzo>
  <uzo tip="fak">GRA</uzo>
  <snc mrk="z.0.FON">
    <dif>
      La dudekoka (kaj lasta) <ref tip="dif"
      cel="grafem.0o">grafemo</ref> de la
      Esperanta alfabeto:
      <ekz>
        nomoj de la literoj: a, bo, co, <klr>[...]</klr>, u, ~uo, vo, zo
        <fnt>
          <bib>F</bib>
          <lok>&FE; 1</lok>
        </fnt>,
      </ekz>
      uzata i.a. kiel:
    </dif>
    <subsnc>
      <dif>
        (maj.) signo de formo: <ekz>Z-fero</ekz>;
      </dif>
      <trd lng="fr">Z<klr> (forme de)</klr></trd>
    </subsnc>
    <subsnc>
      <uzo tip="fak">ESP</uzo>
      <dif> (maj.) mallongigo de <ctl>Zamenhofo</ctl>;
      </dif>
      <trd lng="fr">Z.</trd>
    </subsnc>
    <subsnc>
      <uzo tip="fak">MAT</uzo>
      <dif>(maj. kaj grase) simbolo por la aro de &ccirc;iuj entjeroj
        -2, -1, 0, 1, 2, ...:
        <em>Z</em>;
      </dif>
      <trd lng="fr">Z<klr> (ensemble des entiers)</klr></trd>
    </subsnc>
    <subsnc>
       <uzo tip="fak">FIZ</uzo>
       <dif>
         (min.) simbolo de zepto.
       </dif>
       <trd lng="fr">z.<klr> (zepto)</klr></trd>
    </subsnc>
    <trd lng="fr">z<klr> (lettre)</klr></trd>
  </snc>
</drv>

</art>

