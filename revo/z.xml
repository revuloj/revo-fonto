<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc><rad>z</rad> <fnt><bib>PIV1</bib></fnt>
    </kap>
    <uzo tip="fak">LIN</uzo>
    <uzo tip="fak">GRA</uzo>
    <subart>
      <drv mrk="z.0.konsonanto">
        <kap><tld/></kap>
        <snc>
          <dif>
            La dudek oka (kaj lasta)
            <ref tip="super" cel="liter.0o.propra">litero</ref> de la
            esperanta alfabeto, uzata i.a. kiel:
          </dif>
          <trd lng="fr">z<klr> (lettre)</klr></trd>
          <subsnc mrk="z.0.FON">    
            <uzo tip="fak">FON</uzo>
            <dif>
              En Esperanto kaj en multaj aliaj lingvoj
              latinalafabetaj, signo de
              <ref tip="super" cel="sibl.0anto">sibla</ref>
              <ref tip="super" cel="konson1.0o">konsonanto</ref>, vo&ccirc;a
              <ref tip="super" cel="frot.0sono">frotsono</ref>
              <ref tip="super"
              cel="dent.lang0akonsonanto">lang-denta</ref>
              /z/.
            </dif>
            <trd lng="fr">z<klr> (consonne)</klr></trd>  
          </subsnc>
          <subsnc>
            <dif>
              (maj.) signo de formo: <ekz><tld lit="Z"/>-fero;</ekz>
            </dif>
            <trd lng="fr">Z<klr> (forme de)</klr></trd>
          </subsnc>
          <subsnc>
            <uzo tip="fak">ESP</uzo>
            <dif>
              (maj.) mallongigo de <ctl>Zamenhofo</ctl>: <ekz><tld
              lit="Z"/>;</ekz>
            </dif>
            <trd lng="fr">Zamenhof<klr> (abr&eacute;viation)</klr></trd>
          </subsnc>
          <subsnc>
            <uzo tip="fak">MAT</uzo>
            <dif>
              (maj. grase) simbolo por la aro de &ccirc;iuj entjeroj -2, -1,
              0, 1, 2, ...:
              <ekz><em><tld lit="Z"/></em>;</ekz>
            </dif>
            <trd lng="fr">Z<klr> (ensemble de tous les nombres
                entiers)</klr></trd>
          </subsnc>
          <subsnc>
            <uzo tip="fak">FIZ</uzo>
            <dif>
              (maj.) simbolo de zepto: <ekz><tld lit="Z"/>.</ekz>
            </dif>
            <trd lng="fr">zepto</trd>
          </subsnc>
        </snc>  
      </drv>
    </subart>

    <subart>
      <drv mrk="z.0.nomo">
        <kap><tld/>o</kap>
        <snc>    
          <dif>
            Nomo de tiu <ref tip="super"
            cel="liter.0o.propra">litero</ref>.
          </dif>
          <trd lng="fr">z</trd>
        </snc>
      </drv>
    </subart>
  </art>

