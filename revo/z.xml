<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc><rad>zo</rad> <fnt><bib>PIV1</bib></fnt>
    </kap>
    <uzo tip="fak">LIN</uzo>
    <uzo tip="fak">GRA</uzo>
      <drv mrk="z.0.litero">
        <kap><tld/></kap>
        <snc>
          <dif>
            Nomo de la dudek oka (kaj lasta)
            <ref tip="super" cel="liter.0o.propra">litero</ref> de la
            esperanta alfabeto:
           <ekz>
            <uzo tip="fak">FON</uzo>
            la litero <tld/>
             en Esperanto kaj en multaj aliaj lingvoj
              latinalafabetaj estas signo de
              <ref tip="super" cel="sibl.0anto">sibla</ref>
              <ref tip="super" cel="konson1.0o">konsonanto</ref>, vo&ccirc;a
              <ref tip="super" cel="frot.0sono">frotsono</ref>
              <ref tip="super"
              cel="dent.lang0akonsonanto">lang-denta</ref>;
           </ekz>
           <ekz> 
             <tld/> majuskle estas uzata por
             signo de formo: Z-fero,
             <uzo tip="fak">ESP</uzo>
             mallongigo de <ctl>Zamenhofo</ctl>,
             <uzo tip="fak">MAT</uzo>
              (grase) simbolo por la aro de &ccirc;iuj entjeroj -2, -1,
              0, 1, 2, ...:
              <em>Z</em>;
             <uzo tip="fak">FIZ</uzo>
               simbolo de zepto.
            </ekz>
          </dif>
        </snc>  
        <trdgrp lng="fr">
          <trd>z<klr> (lettre)</klr></trd>,
          <trd>Zamenhof<klr> (abr&eacute;viation)</klr></trd>
          <trd>Z<klr> (ensemble de tous les nombres
               entiers)</klr></trd>
           <trd>zepto</trd>
        </trdgrp>
      </drv>
   </art>

