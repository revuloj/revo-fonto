<drv mrk="pruden.0ulo">
  <kap>
    <tld/>ulo
  </kap>
  <snc>
    <dif>
      Iu <tld/>a:
      <ekz>
        sa&gcirc;ulo a&ubreve;du kaj plimultigu sian scion, kaj <tld/>ulo akiros gvidajn kapablojn
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 1:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la lipoj de <tld/>ulo trovi&gcirc;as sa&gcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 10:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        patro de mal<tld/>ulo ne havos &gcirc;ojon
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 17:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ulo anta&ubreve;vidas malbonon, kaj ka&scirc;i&gcirc;as,
        sed naivuloj anta&ubreve;enpa&scirc;as, kaj difekti&gcirc;as
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 22:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        sen<tld/>uloj en la popolo
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 94:8</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="ant">
     <ref cel="stult.0ulo">stultulo</ref>,
     <ref cel="naiv.0ulo">naivulo</ref>
  </refgrp>
  <trd lng="be">&c_r;&c_a;&c_z;&c_v;&c_a;&c_zh;&c_n;&c_a;&c_ja; &c_a;&c_s;&c_o;&c_b;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Vern&uuml;nftiger</trd>,
    <trd>Kluger</trd>
  </trdgrp>
  <trd lng="ja">&#x826F;&#x8B58;&#x5BB6;  <pr>&#x308A;&#12423;&#12358;&#12375;&#x304D;&#x304B;</pr></trd>
  <trdgrp lng="pl">
    <trd>osoba rozs&aogonek;dna</trd>,
    <trd>roztropny</trd>
  </trdgrp>
</drv>

<drv mrk="pruden.mal0a">
  <kap>mal<tld/>a,
  <var><kap>sen<tld/>a</kap></var></kap>
  <snc mrk="pruden.mal0a.KOMUNE">
    <dif>
      Ne havanta <tld/>on:
      <ekz>
        mal<tld/>a homo neniam konas la mezuron de sia stomako
        <fnt><bib>Fab4</bib>, <lok>Filino de la mar&ccirc;a
            re&gcirc;o</lok></fnt>;
      </ekz>
      <ekz>
        ne estu sen<tld/>a
        <fnt><bib>MT</bib>, <lok>&Pre;, 7:17</lok></fnt>;
      </ekz>
      <ekz>
        kiu estas tiom sen<tld/>a <klr>[&#8230;]</klr> kredi,
        ke ekzistas homoj, kiuj iras kun la piedoj supre kaj
        tenas la kapon malsupren
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
