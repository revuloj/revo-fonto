  </trdgrp>
  <trd lng="uk">&c_z;&c_d;&#x43E;&c_r;&#x43E;&c_v;&c_i;&c_j; &c_g;&#x43B;&c_u;&c_z;&c_d;</trd>
  <trdgrp lng="zh">
    <trd>&#25106;&#x613C; <pr>ji&#xE8;sh&#xE8;n</pr></trd>,
    <trd>&#25106;&#x614E; <pr>ji&#xE8;sh&#xE8;n</pr></trd>,
    <trd>&#29702;&#24615; <pr>l&#x1D0;x&#xEC;ng</pr></trd>,
    <trd>&#x614E;&#x91CD; <pr>sh&#xE8;nzh&#xF2;ng</pr></trd>,
    <trd>&#x60C5;&#29702; <pr>q&#xED;ngl&#x1D0;</pr></trd>
