<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sojl.xml,v 1.26 2023/09/05 06:31:19 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sojl</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sojl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sojl.0o.KON">
    <uzo tip="fak">KON</uzo>
    <dif>
      Transversa &scirc;tono a&ubreve; trabo ku&scirc;anta malsupre de
      la aperturo de
      pordo.
    </dif>
    <trd lng="en">threshold</trd>
    <trd lng="fr">seuil</trd>
    <trdgrp lng="nl">
      <trd>drempel</trd>,
      <trd>dorpel</trd>
    </trdgrp>
  </snc>
  <snc mrk="sojl.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Komenco:
      <ekz>
        <ind>stari sur la <tld/>o</ind>
        <uzo tip="stl">FIG</uzo>
        <klr>(esti tuj okazonta)</klr>.
        <trd lng="hu">k&uuml;sz&ouml;b&ouml;n &aacute;ll</trd>
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>d&eacute;but</trd>,
      <trd>commencement</trd>
    </trdgrp>
    <trd lng="nl">drempel</trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_o;&c_g;</trd>
  <trd lng="bg">&c_p;&c_r;&c_a;&c_g;</trd>
  <trd lng="ca">llindar</trd>
  <trdgrp lng="cs">
    <trd>doln&iacute; mezn&iacute; hodnota</trd>,
    <trd>po&ccaron;&aacute;tek</trd>,
    <trd>pr&aacute;h</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schwelle</trd>,
    <trd>Anfang</trd>,
    <trd>Beginn</trd>
  </trdgrp>
  <trd lng="es">umbral</trd>
  <trd lng="hu">k&uuml;sz&ouml;b</trd>
  <trd lng="it">soglia</trd>
  <trdgrp lng="ja">
    <trd>&#25975;&#x5C45;  [<ind>&#12375;&#x304D;&#12356;</ind>]</trd>,
    <trd>&#38272;&#x53E3;  [<ind>&#x304B;&#12393;&#12368;&#12385;</ind>]</trd>,
    <trd>&#12375;&#x304D;&#12356;&#20516;  [<ind>&#12356;&#x304D;&#12385;</ind>]</trd>,
    <trd>&#x59CB;&#x307E;&#x308A;  [<ind>&#x306F;&#12376;&#x307E;&#x308A;</ind>]</trd>,
    <trd>&#x51FA;&#x767A;&#x70B9;  [<ind>&#12375;&#12421;&#12387;&#12401;&#12388;&#12390;&#12435;</ind>]</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_r;&c_o;&c_g;</trd>
  <trdgrp lng="sk">
    <trd>prah</trd>,
    <trd>za&ccaron;iatok</trd>
  </trdgrp>
  <trd lng="sv">tr&ouml;skel</trd>
  <trd lng="uk">&#x43F;&#x43E;&c_r;&c_ib;&c_g;</trd>
</drv>

<drv mrk="sojl.0a">
  <kap><tld/>a</kap>
  <snc mrk="sojl.0a.KON">
    <dif>
     Rilata al <tld/>o.
     </dif>
  </snc>
  <snc mrk="sojl.0a.SCI">
    <uzo tip="fak">SCI</uzo>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Rilata al lima valoro, punkto, stato a&ubreve; kvanto, kie iu
      fenomeno a&ubreve;
      esenca &scirc;an&gcirc;i&gcirc;o ekokazas:
      <ekz>
        <tld/>a punkto, temperaturo
        <fnt><bib>Viki</bib></fnt>
      </ekz>
    </dif>
    <trd lng="en">critical</trd>
    <trd lng="it">critico</trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_o;&c_g;&c_a;&c_v;&c_y;</trd>
  <trd lng="bg">&c_p;&c_r;&c_a;&c_g;&c_o;&c_v;</trd>
  <trd lng="ja">&#x81E8;&#x754C;&#x306E;  [<ind>&#x308A;&#12435;&#x304B;&#12356;&#x306E;</ind>]</trd>
</drv>

<drv mrk="sojl.0ede">
  <kap><tld/>e de,
  <var><kap>&ccirc;e la <tld/>o de</kap></var></kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="sojl.0ede.KOMUNE">
    <dif>
      &Ccirc;e la komenci&gcirc;o de..., je la ina&ubreve;guro de...:
      <ekz>
        pekinaj esperantistoj kunvenis <tld/>e de la &ccirc;ina novjaro
        <fnt>
          <aut>Usui H.</aut>,
          <vrk><url ref="http://esperanto.china.org.cn/2015-02/09/content_34774578.htm">
          Pekinaj esperantistoj...</url></vrk>,
          <lok>Esperanto.china.org.cn, 2015-02-09</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la <tld/>o de la jaro, de la vintro, de la maljuneco;
      </ekz>
      <ekz>
        &ccirc;e la <tld/>o de la dezerto.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a; &c_p;&c_a;&c_r;&c_o;&c_z;&c_je;&#8230;</trd>
  <trd lng="bg">&c_n;&c_a; &c_p;&c_r;&c_a;&c_g;&c_a; &c_n;&c_a;</trd>
  <trd lng="fr">au seuil de</trd>
  <trd lng="ja">&#xFF5E;<ind>&#x306E;&#x306F;&#12376;&#12417;&#x306B;&#12354;&#x305F;&#x308A;</ind></trd>
</drv>

<drv mrk="sojl.supra0o">
  <kap>supra <tld/>o<fnt>Z</fnt></kap>
  <snc mrk="sojl.supra0o.KON">
    <dif>
      &Scirc;tono a&ubreve; trabo formanta la plafonon de aperturo;
      <ref tip="sin" cel="lintel.0o.ARKI">lintelo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_r;&c_sh;&c_n;&c_ib;&c_k;</trd>
  <trd lng="es">dintel</trd>
  <trd lng="fr">linteau</trd>
  <trd lng="hu">szem&ouml;ld&ouml;kfa</trd>
  <trd lng="ja">&#x304B;&#12418;&#12356;</trd>
  <trd lng="nl">bovendwarsstuk<klr tip="ind"> (v.deur of
  raam)</klr></trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_t;&c_o;&c_l;&c_o;&c_k;&c_a;</trd>
</drv>
</art>
