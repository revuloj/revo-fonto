<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kosmodrom.xml,v 1.12 2022/11/30 17:29:15 revo Exp $">
<kap>
  <rad>kosmodrom</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>

<drv mrk="kosmodrom.0o">
  <kap><tld/>o</kap>
  <snc mrk="kosmodrom.0o.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      Loko kun instala&jcirc;oj por ekflugo de kosmo&scirc;ipoj:
      <ekz>
        Bajkonur estas <tld/>o en Kaza&hcirc;io
        <fnt><bib>Viki</bib>, <lok>&leftquot;kosmodromo Bajkonur&rightquot;,
        2005-10-14</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_s;&c_m;&c_a;&c_d;&c_r;&c_o;&c_m;</trd>
  <trd lng="bg">&c_k;&c_o;&c_s;&c_m;&c_o;&c_d;&c_r;&c_u;&c_m;</trd>
  <trd lng="cs">kosmodrom</trd>
  <trd lng="de">Weltraumbahnhof</trd>
  <trd lng="en">space centre</trd>
  <trdgrp lng="fa">
    <trd>&f_p;&a_A;&f_y;&f_g;&a_A;&a_h; &a_f;&a_D;&a_A;&f_y;&f_y;</trd>,
    <trd>&a_m;&a_r;&f_k;&a_z; &a_f;&a_D;&a_A;&f_y;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">centre spatial</trd>
  <trdgrp lng="it">
    <trd>cosmodromo</trd>,
    <trd>spazioporto</trd>,
    <trd>centro <ind>spaziale</ind></trd>
  </trdgrp>
  <trd lng="nl">ruimtevaartcentrum</trd>
  <trd lng="pl">kosmodrom</trd> 
  <trdgrp lng="pt">
    <trd>cosm&oacute;dromo</trd>,
    <trd>centro <ind>espacial</ind></trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>cosmodrom</trd>,
    <trd>cosmodromul</trd>
  </trdgrp> 
  <trd lng="sk">kozmodr&oacute;m</trd>
</drv>
</art>

