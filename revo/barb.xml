<drv mrk="barb.0a">
  <kap><tld/>a
  <fnt><bib>PV</bib></fnt>,
  <var><kap><tld/>hava</kap></var></kap>
  <snc mrk="barb.0a.KOS">
    <dif>
      Havanta <tld/>on:
      <ekz>
        re&gcirc;a trono, sur kiu sidis grava <tld/>hava persono,
        volvita en longa ru&gcirc;a mantelo
        <fnt>
          <bib>IdO</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a viro sidis tie, proksime al la sola fenestro de la kabano
        <fnt>
          <aut>B. Vikstr&ouml;m, trad. F. Szil&aacute;gyi</aut>
          <vrk>Bona arbarlaboro</vrk>
          <lok>en: Vespera ru&gcirc;o anoncas ventegon, 1950</lok>
        </fnt>;
      </ekz>
      <ekz>
        kupro<tld/>a simio
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj kaprinoj
        <fnt>
          <bib>SrT</bib>
          <lok>Libro IV.</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_a;&c_r;&c_a;&c_d;&c_a;&c_t;&c_y;</trd>
    <trd lng="br">barvek</trd>
    <trd lng="ca">barbat</trd>
    <trd lng="cs">vousat&yacute;</trd>
    <trd lng="de">b&auml;rtig</trd>
    <trd lng="en">bearded</trd>
    <trdgrp lng="es">
      <trd>barbado, -a</trd>,
      <trd>barbudo, -a</trd>
    </trdgrp>
    <trd lng="fr">barbu<klr tip="ind"> (adj.)</klr></trd>
    <trd lng="he">&mem;&zayin;&vav;&qof;&fnun;</trd>
    <trd lng="hu">szak&aacute;llas</trd>
    <trd lng="it">barbuto<klr> (agg.)</klr></trd>
    <trd lng="nl">baardig</trd>
    <trdgrp lng="pl">
      <trd>brodaty</trd>,
      <trd>zaro&sacute;ni&eogonek;ty</trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_o;&c_r;&c_o;&c_d;&c_a;&c_t;&c_y;&c_j;</trd>
    <trd lng="sk">bradat&yacute;</trd>
    <trd lng="sv">sk&auml;ggig</trd>
    <trdgrp lng="zh">
    <trd>&#x6709;&#x5927;&#x80E1;&#x5B50; <pr>y&#x1D2;ud&#xE0;ih&#xFA;zi</pr></trd>,
    <trd>&#x6709;&#x80E1;&#x5B50; <pr>y&#x1D2;uh&#xFA;z&#x1D0;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="cs">vousat&yacute;</trd>
  <trd lng="ja">&#x3072;&#x3052;&#x306E;&#x751F;&#x3048;&#x305F;  <pr>&#x3072;&#x3052;&#x306E;&#x306F;&#x3048;&#x305F;</pr></trd>
  <trd lng="uk">&c_b;&c_o;&c_r;&c_o;&c_d;&c_a;&c_t;&c_i;&c_j;</trd>
</drv>

<drv mrk="barb.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="barb.0isto.barbiro">
    <dif>
      <ref tip="dif" cel="barbir.0o">Barbiro</ref>:
      <ekz>
        ridema viro kun ru&gcirc;a viza&gcirc;o, kiu pli similis <tld/>iston ol skribiston
        <fnt>
          <bib>StU</bib>
          <lok>La Nazaretanoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_l;&c_ja;&c_r;</trd>,
    <trd>&c_c;&c_y;&c_r;&c_u;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="br">barver</trd>
  <trd lng="ca">barber</trd>
  <trd lng="cs">holi&ccaron;</trd>
  <trd lng="de">Bartschneider</trd>
  <trd lng="en">barber</trd>
  <trd lng="es">barbero</trd>
  <trd lng="fr">barbier</trd>
  <trd lng="he">&samekh;&#x5B7;&pe;&#x5BC;&#x5B8;&resh;</trd>
  <trd lng="it">barbiere</trd>
  <trd lng="nl">barbier</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_a;&c_d;&c_o;&c_b;&c_r;&c_je;&c_j;</trd>,
    <trd>&c_c;&c_i;&c_r;&c_ju;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">holi&ccaron;</trd>
  <trd lng="sv">barberare</trd>
</drv>

<drv mrk="barb.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="barb.0ulo.FRIZ">
    <dif>
      <tld lit="B"/>hava homo:
      <ekz>
        venis <tld/>ulo, kiu montris al mi multe da mono
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fabelo pri iu, kiu migris por ekkoni timon</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>ulo fi&scirc;kaptis tie <klr>[&#x2026;]</klr> kaj malfeli&ccirc;e la vento implikis lian
        <tld/>on kun la &scirc;nuro
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino kaj Rozulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la blank<tld/>ulo &ccirc;iam estas afabla al &scirc;i
        <fnt>
          <bib>IdO</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        menton<tld/>ulo
        <fnt>
          <bib>IdO</bib>
          <lok>Kvina Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        iu ebria <ind>griz<tld/>ulo</ind> komencis instrui min pri la vivo
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
        <trd lng="de">Graubart</trd>
      </ekz>
      <ekz>
        kelkaj <nom>ru&gcirc;barbulo</nom>j en la komunumo aludadis, ke la rabeno edzi&gcirc;is
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
        <trd lng="de">Rotbart</trd>
      </ekz>
      <ekz>
        en Japanio estas tre malmultaj <tld/>uloj
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="&Monato;/2010/010101.php">Barb-ara decido</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_a;&c_r;&c_a;&c_d;&c_a;&c_ch;</trd>
    <trdgrp lng="de">
      <trd>Barttr&auml;ger</trd>,
      <trd>B&auml;rtiger</trd>
    </trdgrp>
    <trd lng="ca">barbut</trd>
    <trd lng="br">barveg</trd>
    <trd lng="en">bearded (individual)</trd>
    <trd lng="es">barbudo</trd>
    <trd lng="fr">barbu<klr tip="ind"> (subst.)</klr></trd>
    <trd lng="hu">szak&aacute;llas<klr tip="ind"> (ember)</klr></trd>
    <trd lng="it">barbuto<klr> (sost.)</klr></trd>
    <trd lng="nl">baardman</trd>
    <trd lng="pl">brodacz</trd>
    <trd lng="ru">&c_b;&c_o;&c_r;&c_o;&c_d;&c_a;&c_ch;</trd>
    <trd lng="sk">brad&aacute;&ccaron;</trd>
    <trd lng="sv">sk&auml;ggig person</trd>
  </snc>
  <trd lng="cs">vous&aacute;&ccaron;</trd>
  <trd lng="he">&mem;&zayin;&vav;&qof;&fnun;</trd>
  <trd lng="ja">&#x3072;&#x3052;&#x3092;&#x751F;&#x3084;&#x3057;&#x305F;&#x4EBA;  <pr>&#x3072;&#x3052;&#x3092;&#x306F;&#x3084;&#x3057;&#x305F;&#x3058;&#x3093;</pr></trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_o;&c_r;&c_o;&c_d;&c_a;&c_t;&c_i;&c_j; &c_ch;&c_o;&c_l;&c_o;&c_v;&c_ib;&c_k;</trd>,
    <trd>&c_b;&c_o;&c_r;&c_o;&c_d;&c_a;&c_n;&c_mol;</trd>,
    <trd>&c_b;&c_o;&c_r;&c_o;&c_d;&c_a;&c_ch;</trd>
  </trdgrp>
</drv>

<drv mrk="barb.sen0a">
  <kap>sen<tld/>a</kap>
  <snc>
    <dif>
      Ne havanta <tld/>on:
      <ekz>
        la rondaj, sen<tld/>aj viza&gcirc;oj de la armitaj e&ubreve;nukoj
        <fnt>
        <bib>I&scirc;tar</bib>,
        <lok>&ccirc;apitro 21a</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia sen<tld/>a viza&gcirc;o surhavis &ccirc;iujn postsignojn de supermezura uzado de
        alkohola&jcirc;o
        <fnt>
          <bib>IdO</bib>
          <lok>Kvara Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_je;&c_z;&c_b;&c_a;&c_r;&c_o;&c_d;&c_y;</trd>
    <trdgrp lng="br">
      <trd>divarv</trd>,
      <trd>blouc'h</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>barbamec</trd>,
      <trd>imberbe</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>bartlos</trd>,
      <trd>glatt</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>beardless</trd>,
      <trd>clean-<ind>shaven</ind></trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>desbarbado, -a</trd>,
      <trd>chino, -a<klr> (El Salv.)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>glabre</trd>,
      <trd>imberbe</trd>
    </trdgrp>
    <trd lng="hu">csupasz k&eacute;p&udblac;</trd>
    <trd lng="it">imberbe</trd>
    <trd lng="nl">baardloos</trd>
    <trd lng="ru">&c_b;&c_je;&c_z;&c_b;&c_o;&c_r;&c_o;&c_d;&c_y;&c_j;</trd>
    <trdgrp lng="sv">
      <trd>sk&auml;ggl&ouml;s</trd>,
      <trd>sl&auml;trakad</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>bezvous&yacute;</trd>,
    <trd>holobrad&yacute;</trd>
  </trdgrp>
  <trd lng="sk">holobrad&yacute;</trd>
</drv>

