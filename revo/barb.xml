<kap>
  <ofc>*</ofc>
  <rad>barb</rad>/o
</kap>
<drv mrk="barb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="barb.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Haroj de vango kaj mentono:
      <ekz>
        &ccirc;iuj kapoj estas kalvaj, &ccirc;iuj <tld/>oj estas razitaj
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 15:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        eksterordinare longa <tld/>o, kiu estis pli ru&gcirc;a ol blanka
        <fnt>
          <bib>Fab2</bib>
          <lok>Ru&gcirc;aj &scirc;uoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        kapra <tld/>o
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        plej rimarkinda estis lia grandega peruko <klr>[&#x2026;]</klr> kaj arta
        <tld/>o, similanta katan voston
        <fnt><bib>Far1</bib> <lok>&ccirc;apitro 1a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>o elkreskis, sed sa&gcirc;on ne naskis
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_a;&c_r;&c_a;&c_d;&c_a;</trd>
    <trd lng="br">barv</trd>
    <trd lng="ca">barba</trd>
    <trd lng="de">Bart</trd>
    <trd lng="en">beard</trd>
    <trd lng="es">barba</trd>
    <trd lng="fr">barbe</trd>
    <trd lng="he">&zayin;&qof;&fnun;</trd>
    <trd lng="hu">szak&aacute;ll</trd>
    <trd lng="id">janggut</trd>
    <trd lng="it">barba</trd>
    <trd lng="nl">baard</trd>
    <trdgrp lng="pl">
      <trd>broda</trd>,
      <trd>zarost</trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_o;&c_r;&c_o;&c_d;&c_a;</trd>
    <trd lng="sk">brada</trd>
    <trd lng="sv">sk&auml;gg</trd>
    <trd lng="vo">balib</trd>
    <trd lng="zh">&#x80E1;&#x5B50; <pr>h&#xFA;zi</pr></trd>
</snc>
  <trd lng="bo">&t_ra;&t_gata;&t_yata;&t_tsheg;</trd>
  <trd lng="cs">vousy na brad&ecaron; a l&iacute;c&iacute;ch</trd>
  <trdgrp lng="ja">
    <trd>&#x3042;&#x3054;&#x3072;&#x3052;</trd>,
    <trd>&#x307B;&#x304A;&#x3072;&#x3052;</trd>
  </trdgrp>
  <trd lng="uk">&c_b;&c_o;&c_r;&c_o;&c_d;&c_a;</trd>
</drv>

