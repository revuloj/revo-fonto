<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: barb.xml,v 1.36 2022/04/26 15:00:32 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>barb</rad>/o
</kap>
<drv mrk="barb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="barb.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Haroj de vango kaj mentono:
      <ekz>
        kiel pastro, li razis la <tld/>on kaj la kapon
        <fnt><bib>Far1</bib> <lok>&ccirc;apitro 1a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>o de viro, kapro;
      </ekz>
      <ekz>
        falsa, artefarita <tld/>o;
      </ekz>
      <ekz>
        plej rimarkinda estis lia grandega peruko [...] kaj arta
        <tld/>o,
        similanta katan voston
        <fnt><bib>Far1</bib> <lok>&ccirc;apitro 1a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>o elkreskis, sed sa&gcirc;on ne naskis
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_a;&c_r;&c_a;&c_d;&c_a;</trd>
    <trd lng="br">barv</trd>
    <trd lng="ca">barba</trd>
    <trd lng="de">Bart</trd>
    <trd lng="en">beard</trd>
    <trd lng="es">barba</trd>
    <trd lng="fr">barbe</trd>
    <trd lng="he">&zayin;&qof;&fnun;</trd>
    <trd lng="hu">szak&aacute;ll</trd>
    <trd lng="id">janggut</trd>
    <trd lng="it">barba</trd>
    <trd lng="nl">baard</trd>
    <trdgrp lng="pl">
      <trd>broda</trd>,
      <trd>zarost</trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_o;&c_r;&c_o;&c_d;&c_a;</trd>
    <trd lng="sk">brada</trd>
    <trd lng="sv">sk&auml;gg</trd>
    <trd lng="vo">balib</trd>
  </snc>
  <trd lng="bo">&t_ra;&t_gata;&t_yata;&t_tsheg;</trd>
  <trd lng="cs">vousy na brad&ecaron; a l&iacute;c&iacute;ch</trd>
  <trd lng="uk">&#x431;&#x43E;&#x440;&#x43E;&#x434;&#x430;</trd>
</drv>

<drv mrk="barb.0ajxo">
  <kap><tld/>a&jcirc;o<fnt>PV</fnt></kap>
  <snc>
    <dif>
      Io simila al <tld/>o.
    </dif>
    <trd lng="de">Barthaar</trd>
    <trd lng="es">barba</trd>
    <trd lng="nl">baardhaar</trd>
  </snc>
  <adm>
    La vorto ne plu estas en PIV. &Gcirc;ia senco
    ne aperas klare. (MB)
  </adm>
  <trd lng="he">&zayin;&#x5B8;&qof;&#x5B8;&fnun;</trd>
</drv>

<drv mrk="barb.0a">
  <kap><tld/>a<fnt>PV</fnt>,
  <var><kap><tld/>hava</kap></var></kap>
  <snc mrk="barb.0a.KOS">
    <dif>
      Havanta <tld/>on:
      <ekz>
        <tld/>a viro sidis tie, proksime al la sola fenestro de la kabano
        <fnt>
          <aut>B. Vikstr&ouml;m, trad. F. Szil&aacute;gyi</aut>
          <vrk>Bona arbarlaboro</vrk>
          <lok>en: Vespera ru&gcirc;o anoncas ventegon, 1950</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_a;&c_r;&c_a;&c_d;&c_a;&c_t;&c_y;</trd>
    <trd lng="br">barvek</trd>
    <trd lng="ca">barbat</trd>
    <trd lng="cs">vousat&yacute;</trd>
    <trd lng="de">b&auml;rtig</trd>
    <trd lng="en">bearded</trd>
    <trdgrp lng="es">
      <trd>barbado, -a</trd>,
      <trd>barbudo, -a</trd>
    </trdgrp>
    <trd lng="fr">barbu<klr tip="ind"> (adj.)</klr></trd>
    <trd lng="he">&mem;&zayin;&vav;&qof;&fnun;</trd>
    <trd lng="hu">szak&aacute;llas</trd>
    <trd lng="it">barbuto<klr> (agg.)</klr></trd>
    <trd lng="nl">baardig</trd>
    <trdgrp lng="pl">
      <trd>brodaty</trd>,
      <trd>zaro&sacute;ni&eogonek;ty</trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_o;&c_r;&c_o;&c_d;&c_a;&c_t;&c_y;&c_j;</trd>
    <trd lng="sk">bradat&yacute;</trd>
    <trd lng="sv">sk&auml;ggig</trd>
  </snc>
  <trd lng="cs">vousat&yacute;</trd>
  <trd lng="uk">&#x431;&#x43E;&#x440;&#x43E;&#x434;&#x430;&#x442;&#x438;&#x439;</trd>
</drv>

<drv mrk="barb.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="barb.0isto.barbiro">
    <dif>
      <ref tip="dif" cel="barbir.0o">Barbiro</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_l;&c_ja;&c_r;</trd>,
    <trd>&c_c;&c_y;&c_r;&c_u;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="br">barver</trd>
  <trd lng="ca">barber</trd>
  <trd lng="cs">holi&ccaron;</trd>
  <trd lng="de">Bartschneider</trd>
  <trd lng="en">barber</trd>
  <trd lng="es">barbero</trd>
  <trd lng="fr">barbier</trd>
  <trd lng="he">&samekh;&#x5B7;&pe;&#x5BC;&#x5B8;&resh;</trd>
  <trd lng="it">barbiere</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_a;&c_d;&c_o;&c_b;&c_r;&c_je;&c_j;</trd>,
    <trd>&c_c;&c_i;&c_r;&c_ju;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">holi&ccaron;</trd>
  <trd lng="sv">barberare</trd>
</drv>

<drv mrk="barb.0lano">
  <kap><tld/>lano<fnt>Z</fnt></kap>
  <snc>
    <dif>
      <ref tip="dif" cel="lanug.0o.junulo">Lanugo<sncref/></ref>.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_sh;&c_o;&c_k; <klr tip="amb">(&c_n;&c_a;
  &c_p;&c_a;&c_d;&c_b;&c_a;&c_r;&c_o;&c_d;&c_z;&c_mol;&c_d;&c_z;&c_ib;)</klr></trd>
  <trd lng="br">marblev</trd>
  <trd lng="ca">p&egrave;l moix&iacute;</trd>
  <trd lng="cs">chm&yacute;&rcaron;&iacute; na brad&ecaron;</trd>
  <trd lng="en">fuzz</trd>
  <trd lng="fr">duvet<klr tip="ind"> (poils)</klr></trd>
  <trd lng="hu">sz&odblac;rpihe</trd>
  <trd lng="it">lanuggine</trd>
  <trd lng="ru">&c_p;&c_u;&c_sh;&c_o;&c_k; <klr tip="amb">(&c_n;&c_a;
  &c_p;&c_o;&c_d;&c_b;&c_o;&c_r;&c_o;&c_d;&c_k;&c_je;)</klr></trd>
  <trd lng="sk">jemn&eacute; ch&lacute;pky na brade</trd>
  <trd lng="sv">fjun</trd>
</drv>

<drv mrk="barb.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="barb.0ulo.FRIZ">
    <dif>
      <tld lit="B"/>hava homo.
    </dif>
    <trd lng="be">&c_b;&c_a;&c_r;&c_a;&c_d;&c_a;&c_ch;</trd>
    <trdgrp lng="de">
      <trd>Barttr&auml;ger</trd>,
      <trd>B&auml;rtiger</trd>
    </trdgrp>
    <trd lng="ca">barbut</trd>
    <trd lng="br">barveg</trd>
    <trd lng="en">bearded (individual)</trd>
    <trd lng="es">barbudo</trd>
    <trd lng="fr">barbu<klr tip="ind"> (subst.)</klr></trd>
    <trd lng="hu">szak&aacute;llas<klr tip="ind"> (ember)</klr></trd>
    <trd lng="it">barbuto<klr> (sost.)</klr></trd>
    <trd lng="nl">baardman</trd>
    <trd lng="pl">brodacz</trd>
    <trd lng="ru">&c_b;&c_o;&c_r;&c_o;&c_d;&c_a;&c_ch;</trd>
    <trd lng="sk">brad&aacute;&ccaron;</trd>
    <trd lng="sv">sk&auml;ggig person</trd>
  </snc>
  <trd lng="cs">vous&aacute;&ccaron;</trd>
  <trd lng="he">&mem;&zayin;&vav;&qof;&fnun;</trd>
  <trdgrp lng="uk">
    <trd>&#x431;&#x43E;&#x440;&#x43E;&#x434;&#x430;&#x442;&#x438;&#x439; &#x447;&#x43E;&#x43B;&#x43E;&#x432;&#x456;&#x43A;</trd>,
    <trd>&#x431;&#x43E;&#x440;&#x43E;&#x434;&#x430;&#x43D;&#x44C;</trd>,
    <trd>&#x431;&#x43E;&#x440;&#x43E;&#x434;&#x430;&#x447;</trd>
  </trdgrp>
</drv>

<drv mrk="barb.0fisxo">
  <kap><tld/>fi&scirc;o<fnt>PV</fnt></kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Riverfi&scirc;o el familio karpoidoj (<trd lng="la">barbus</trd>).
    </dif>
  </snc>
  <adm>
    PIV enkondukis "barbio" por &ccirc;i tiu senco. (MB)
  </adm>
    <trdgrp lng="de">
      <trd>Barbe</trd>,
      <trd>Bartenfisch</trd>
    </trdgrp>
    <trd lng="ca">barb</trd>
    <trd lng="en">catfish</trd>
    <trd lng="fr">barbeau</trd>
    <trd lng="hu">m&aacute;rna</trd>
    <trd lng="it">barbo</trd>
    <trd lng="pl">brzanka</trd>
    <trd lng="ru">&c_u;&c_s;&c_a;&c_ch; <klr tip="amb">(&c_r;&c_y;&c_b;&c_a;)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x430;&#x440;&#x435;&#x43D;&#x430;</trd>,
    <trd>&#x432;&#x443;&#x441;&#x430;&#x447;</trd>
  </trdgrp>
</drv>

<drv mrk="barb.lip0o">
  <kap>
    lip<tld/>o
  </kap>
  <snc mrk="barb.lip0o.FRIZ">
    <dif>
      <ref tip="dif" cel="har.lip0aro">Liphararo</ref>:
      <ekz>
        <nom>Janni</nom> kuris por lin venigi, kaj alkondukis
        lin tute dormantan, kun malfrizita
        lip<tld/>o, kaj kun kapo zorge envolvita en naztuko
        <fnt>
          <aut>E. About, trad. G. Moch</aut>
          <vrk><url ref="&Gutenberg;files/65835/65835-0.txt">La Re&gcirc;o de la Montoj</url></vrk>
          <lok>1909</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_s;&c_y;</trd>
  <trd lng="bg">&c_m;&c_u;&c_s;&c_t;&c_a;&c_k;</trd>
  <trd lng="br">mourro&ugrave;</trd>
  <trd lng="cs">kn&iacute;r</trd>
  <trdgrp lng="de">
    <trd>Oberlippenbarthaare</trd>,
    <trd>Schnauzer</trd>,
    <trd>Bart<klr>(haare)</klr></trd>
  </trdgrp>
  <trd lng="el">&my;&omikron;&ypsilon;&sigma;&tau;&alfa_ton;&kappa;&jota;</trd>
  <trd lng="en">moustache</trd>
  <trd lng="es">bigote</trd>
  <trd lng="fr">moustache</trd>
  <trd lng="he">&shin;&pe;&fmem;</trd>
  <trd lng="hu">bajusz</trd>
  <trd lng="id">kumis</trd>
  <trd lng="it">baffi</trd>
  <trd lng="nl">snor</trd>
  <trd lng="pl">w&aogonek;sy</trd>
  <trd lng="pt">bigode</trd>
  <trd lng="ru">&c_u;&c_s;&c_y;</trd>
  <trd lng="sk">f&uacute;zy</trd>
  <trd lng="sv">mustasch</trd>
</drv>

<drv mrk="barb.sen0a">
  <kap>sen<tld/>a</kap>
  <snc>
    <dif>
      Ne havanta <tld/>on:
      <ekz>
        la rondaj, senbarbaj viza&gcirc;oj de la armitaj e&ubreve;nukoj
        <fnt>
        <bib>I&scirc;tar</bib>,
        <lok>&ccirc;apitro 21a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_je;&c_z;&c_b;&c_a;&c_r;&c_o;&c_d;&c_y;</trd>
    <trdgrp lng="br">
      <trd>divarv</trd>,
      <trd>blouc'h</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>barbamec</trd>,
      <trd>imberbe</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>bartlos</trd>,
      <trd>glatt</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>beardless</trd>,
      <trd>clean-<ind>shaven</ind></trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>desbarbado, -a</trd>,
      <trd>chino, -a<klr> (El Salv.)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>glabre</trd>,
      <trd>imberbe</trd>
    </trdgrp>
    <trd lng="hu">csupasz k&eacute;p&udblac;</trd>
    <trd lng="it">imberbe</trd>
    <trd lng="nl">baardloos</trd>
    <trd lng="ru">&c_b;&c_je;&c_z;&c_b;&c_o;&c_r;&c_o;&c_d;&c_y;&c_j;</trd>
    <trdgrp lng="sv">
      <trd>sk&auml;ggl&ouml;s</trd>,
      <trd>sl&auml;trakad</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>bezvous&yacute;</trd>,
    <trd>holobrad&yacute;</trd>
  </trdgrp>
  <trd lng="sk">holobrad&yacute;</trd>
</drv>
</art>
