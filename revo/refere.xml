<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>referenc</rad>/o
</kap>
<drv mrk="refere.0o">
  <kap><tld/>o</kap>
  <snc mrk="refere.0o.bibliografia">
    <dif>
      Atentigo pri originala pruva a&ubreve; utila teksto:
      <ekz>
        la &jcirc;us aperinta bibliografio enhavas <tld/>ojn al iom pli ol 2000
        tradukoj
        <fnt>
        <aut>Paul Peeraerts</aut>,
        <vrk>Kiu sekvos? (William Auld kaj E. G. Evans, Bibliografio de
        tradukoj el la angla lingvo)</vrk>,
        <bib>Monato</bib>, <lok>jaro 1996a, numero 6a, p. 20a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="biblio.0o">bibliografio</ref>,
      <ref cel="cit.0ajxo">cita&jcirc;o</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_s;&c_p;&c_a;&c_s;&c_y;&c_l;&c_k;&c_a;</trd>,
      <trd>&c_u;&c_k;&c_a;&c_z;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Verweis</trd>,
      <trd>Quellenangabe</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>r&eacute;f&eacute;rence<klr> (bibliographique)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>hivatkoz&aacute;s</trd>,
      <trd>referencia</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>referentie</trd>,
      <trd>bronvermelding</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_s;&c_y;&c_l;&c_k;&c_a;<klr tip="ind"> (&c_u;&c_k;&c_a;&c_z;&c_a;&c_t;&c_je;&c_l;&c_mol;)</klr></trd>,
      <trd>&c_o;&c_t;&c_s;&c_y;&c_l;&c_k;&c_a;<klr tip="ind"> (&c_u;&c_k;&c_a;&c_z;&c_a;&c_t;&c_je;&c_l;&c_mol;)</klr></trd>,
      <trd>&c_u;&c_k;&c_a;&c_z;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="refere.0o.rekomendo">
    <dif>
      Rekomenda atesto.
    </dif>
    <ref tip="vid" cel="leter.rekomend0o">rekomendletero</ref>
    <trdgrp lng="be">
      <trd>&c_r;&c_e;&c_k;&c_a;&c_m;&c_e;&c_n;&c_d;&c_a;&c_c;&c_y;&c_ja;</trd>,
      <trd>&c_r;&c_e;&c_k;&c_a;&c_m;&c_e;&c_n;&c_d;&c_a;&c_c;&c_y;&c_j;&c_n;&c_y; &c_l;&c_ib;&c_s;&c_t;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Referenz</trd>,
      <trd>Empfehlungsschreiben</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>r&eacute;f&eacute;rence<klr> (recommandation)</klr></trd>
    </trdgrp>
    <trd lng="hu">referencia</trd>
    <trd lng="nl">referentie</trd>
  </snc>
</drv>

<drv mrk="refere.0i">
  <kap><tld/>i</kap>
  <snc>
    <dif>
      Prezenti indikon pri fonta, apoga dokumento:
      <ekz>
        la komentoj <klr>...</klr> donas
        botanikajn informojn, <tld/>ante al a&ubreve;toroj kaj klasikaj kaj
        deksesjarcentaj
        <fnt>
        <aut>Johan Van Eenoo</aut>,
        <vrk>La bru&gcirc;a Centuriae plantarum rariorum retrovita en
        Krakovo</vrk>,
        <bib>Monato</bib>, <lok>jaro 1998a, numero 1a, p. 16a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_a;&c_s;&c_l;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_d;&c_a;&c_c;&c_mol; <ind>&c_s;&c_p;&c_a;&c_s;&c_y;&c_l;&c_k;&c_u;</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>r&eacute;f&eacute;rer</ind> &agrave;</trd>,
    <trd>faire <ind>r&eacute;f&eacute;rence</ind> &agrave;</trd>,
    <trd>mentionner<klr> (une r&eacute;f&eacute;rence)</klr></trd>
  </trdgrp>
</drv>
</art>
