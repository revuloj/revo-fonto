<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kipr.xml,v 1.21 2023/12/03 09:58:49 revo Exp $">
<kap>
  <rad>Kipr</rad>/o <fnt><bib>NT</bib></fnt>,
  <var><kap><rad var="C">Cipr</rad>/o <fnt><bib>QuV</bib></fnt></kap></var>
</kap>

<drv mrk="kipr.K0o">
  <kap>
    <tld lit="K"/>o, <var><kap><tld var="C"/>o</kap></var>
  </kap>

  <snc mrk="kipr.K0o.insulo">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" cel="insul.0o" lst="voko:insuloj">Insulo</ref>
      en <ref tip="vid" cel="mediterane.M0o.GEOG">Mediteranea
      Maro</ref>:
      <ekz>
        <klr>[ili]</klr> voja&gcirc;is &gcirc;is Fenikio kaj <tld/>o kaj Antio&hcirc;ia
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 11:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekvidinte <tld/>on kaj lasinte &gcirc;in maldekstre, ni veturis al Sirio, kaj el&scirc;ipi&gcirc;is en Tiro
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 21:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        en Pafoso, sur <tld var="C"/>o, estas templo, sinjoro, kie oni konservas zoneton de Venuso
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        kian vinon oni sendis al mi de <tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>!
      </ekz>
      <ekz>
        <tld/>ano la&ubreve; sia naski&gcirc;o
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 4:36</lok>
        </fnt>;
      </ekz>
      <ekz>
        de <tld var="C"/>o &gcirc;is Jafo-urbo estas 400 verstoj tra la maro
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 11 (61)</lok>
        </fnt>;
      </ekz>
      <ekz>
         konfliktaj a&ubreve; problemaj teritorioj: <klr>[&#x2026;]</klr> norda turka <tld/>o, Karaba&hcirc;o, E&ubreve;skio,
         <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Sergeo Yurjich</aut>
          <vrk><url ref="&Monato;/2008/009410.php">Kiuj sendependi&gcirc;u?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>Cypr</trd>,
    </trdgrp>
  </snc>
  <snc mrk="kipr.K0o.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <mlg>CY</mlg>
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
    <dif>
      <ref tip="lst" cel="sxtat.0o" lst="voko:&scirc;tatoj">&Scirc;tato</ref>
      en tiu insulo kun &ccirc;efurbo Nikosio,
      teritorio de Respubliko <tld/>o:
      <ekz>
        unu a&ubreve; pluraj el la unuaj ses kandidataj &scirc;tatoj: <tld/>o, &Ccirc;e&hcirc;io, Estonio, Hungario, Pollando
        kaj Slovenio
        <fnt>
       <aut>Marc Vanden Bempt</aut>
          <vrk><url ref="&Monato;/2000/006849.php">Kaj sekve ...</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 11</lok>
        </fnt>;
      </ekz>
      <ekz>
        Slovenio, du baltaj &scirc;tatoj kaj eble <tld/>o akceptos e&ubreve;ron kiel propran valuton plej malfrue
        &gcirc;is 2008
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2004/008396.html">Profunda
          struktur&scirc;an&gcirc;o en meza kaj orienta E&ubreve;ropo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>Cypr</trd>,
      <trd>Republika <ind>Cypryjska</ind></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_K;&c_ib;&c_p;&c_r;</trd>
  <trd lng="bg">&c_K;&c_i;&c_p;&c_malmol;&c_r;</trd>
  <trd lng="de">Zypern</trd>
  <trd lng="en">Cyprus</trd>
  <trd lng="es">Chipre</trd>
  <trd lng="fa">&a_q;&a_b;&a_r;&a_s;</trd>
  <trd lng="fr">Chypre</trd>
  <trd lng="id">Siprus</trd>
  <trd lng="ja">&#x30AD;&#x30D7;&#x30ED;&#x30B9;</trd>
  <trd lng="nl">Cyprus</trd>
  <trd lng="pt">Chipre</trd>
  <trd lng="tr">K&inodot;br&inodot;s</trd>
  <trd lng="uk">&c_K;&c_ib;&c_p;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#x585E;&#x6D66;&#x8DEF;&#x65AF; <pr>S&#xE0;ip&#x1D4;l&#xF9;s&#x12B;</pr></trd>,
    <trd>&#x8CFD;&#x666E;&#x52D2;&#x65AF; <pr>S&#xE0;ip&#x1D4;l&#xE8;s&#x12B;</pr></trd>,
    <trd>&#x585E;&#x6D66;&#x8DEF;&#x65AF; <pr>s&#xE0;ip&#x1D4;l&#xF9;s&#x12B;</pr></trd>,
    <trd>&#x8CFD;&#x666E;&#x52D2;&#x65AF; <pr>s&#xE0;ip&#x1D4;l&#xE8;s&#x12B;</pr></trd>,
    <trd>&#x8CFD;&#x666E;&#x52D2;&#x65AF; <pr>s&#xE0;ip&#x1D4;l&#x113;is&#x12B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kipr.0a">
  <kap>
    <tld lit="k"/>a, <var><kap><tld lit="c"/>a</kap></var>
  </kap>
  <snc>
    <dif>
       Rilata al <tld/>o:
      <ekz>
        malsa&gcirc;a estas tiu, kiu en Kipro ne gustumas <tld lit="k"/>an vinon, sed trinkas tiran bieron
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="k"/>a muflono ekzistas en neniu alia lando de la mondo
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2010/010163.php">&Ccirc;asantoj &ccirc;asataj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ver&scirc;etante la vinon, <klr>[&#x2026;]</klr> honore al la <tld lit="c"/>a diino
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LXXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="k"/>a dialekto
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2011/010183.php">Terasa apartamento</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="k"/>a registaro
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2011/010247.php">Kipro eksportas, &Ccirc;inio importas</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_p;&c_r;&c_s;&c_k;&c_ib;</trd>
  <trd lng="de">zypriotisch</trd>
</drv>

</art>
