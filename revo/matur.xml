  <trd lng="de">&uuml;berreif</trd>
  <trdgrp lng="fr">
    <trd>blet</trd>,
    <trd>trop <ind>m&ucirc;r</ind></trd>
  </trdgrp>
  <trd lng="ja">&#x719F;&#x308C;&#x3059;&#x304E;&#x305F;  [<ind>&#x3046;&#x308C;&#x3059;&#x304E;&#x305F;</ind>]</trd>
