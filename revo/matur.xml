</kap>

<drv mrk="matur.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Atinginta sian plenan kreskadon, forton, kapablon a&ubreve; 
      ta&ubreve;gecon:
      <ekz>
        infano ne estas <tld/>a homo
        <fnt>
          <bib>F</bib>
          <lok>&FE; 6</lok>
        </fnt>;
      </ekz>
      <ekz>
        aperis sur <klr>[la bran&ccirc;o]</klr> beraroj kun <tld/>aj beroj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 40:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        figarboj kun <tld/>aj fruktoj
        <fnt>
          <bib>MT</bib>
          <lok>Na&hcirc;um 3:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;arma aveno! <klr>[&#x2026;]</klr> <tld/>a &gcirc;i aspektas kiel tuta amaso da malgrandaj
        flavaj kanarioj sur unu bran&ccirc;o
        <fnt>
          <bib>Fab2</bib>
          <lok>Poligono</lok>
        </fnt>;
      </ekz>
      <ekz>
        bela kaj <tld/>a knabino
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a beleco de virino jam preter la fruaj jaroj de junula &ccirc;armo
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne sole la infanoj legas kun plezuro la <nom>Grimm</nom>'ajn fabelojn, anka&ubreve; la <tld/>aj
        homoj &scirc;atas ilin
        <fnt>
          <bib>ElektFab</bib>
          <lok>Anta&ubreve;parolo de la tradukinto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a embrio, kiu preme postulas sian naski&gcirc;on
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la jaroj <tld/>aj tiu ideo akiris &ccirc;e li &ccirc;iam pli gravan rolon
        <fnt>
          <bib>Zam</bib>
          <lok>Zamenhofa juna&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        a&gcirc;o tro <tld/>a ne estas plezura
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kresk.plen0a">plenkreska</ref>
    </refgrp>
    <trd lng="sv">mogen</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plene esplorita, provita, finstudita:
      <ekz>
        la teksto definitiva estos fiksita nur iam poste, post <tld/>a komuna
        interkonsili&gcirc;o
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Nefermita letero al s-ro de Beaufront.</vrk>
        </fnt>;
      </ekz>
      <ekz>
        ni devas tion &ccirc;i fari ne facilanime, sed nur <klr>[&#x2026;]</klr> post <tld/>a
        priju&gcirc;ado
        <fnt>
          <bib>F</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        en efektiveco <klr>[tio]</klr> post pli <tld/>a pripenso montri&gcirc;us eble nur kiel
        malplibonigo
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista en Washington en la 15a de a&ubreve;gusto
          1910</vrk>
        </fnt>;
      </ekz>
      <ekz>
        malutili al si per ia ne sufi&ccirc;e <tld/>a dekreto
        <fnt>
          <bib>LR</bib>
          <lok>Unua Parto &ndash; Leksikologio</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a elprovado
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sepa Kongreso Esperantista en Antwerpen en la 21a de a&ubreve;gusto
          1911</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[t]</klr>iam la plej granda parto de la novaj 
        vortoj estos jam tute <tld/>a
        <fnt>
          <bib>F</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="detal.0a.0hava">detala<sncref/></ref>,
      <ref cel="fund.0a.FIG">funda<sncref/></ref>,
      <ref cel="profun.0a.kerna">profunda<sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>reiflich</trd>,
      <trd>ausf&uuml;hrlich</trd>,
      <trd>gr&uuml;ndlich</trd>
    </trdgrp>    
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_p;&c_je;&c_l;&c_y;</trd>,
    <trd>&c_s;&c_a;&c_s;&c_mol;&c_p;&c_je;&c_l;&c_y;</trd>,
    <trd>&c_d;&c_a;&c_r;&c_o;&c_s;&c_l;&c_y; <klr>(&c_p;&c_r;&c_a; 
        &c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;&c_a;)</klr></trd>,
    <trd>&c_s;&c_t;&c_a;&c_l;&c_y; <klr>(&c_p;&c_r;&c_a; 
        &c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="bo">&t_sa;&t_mata;&t_gigu;&t_na;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trdgrp lng="br">
    <trd>azv</trd>,
    <trd>darev</trd>
  </trdgrp>
  <trd lng="ca">madur</trd>
  <trdgrp lng="cs">
  <trd>dosp&ecaron;l&yacute;</trd>,
  <trd>pln&ecaron; vyvinut&yacute;</trd>,
  <trd>vysp&ecaron;l&yacute;</trd>,
  <trd>zral&yacute;</trd>
  </trdgrp>
  <trd lng="de">reif</trd>
  <trdgrp lng="en">
    <trd>mature</trd>,
    <trd>ripe</trd>
  </trdgrp>
  <trd lng="es">maduro</trd>
  <trdgrp lng="fr">
    <trd>m&ucirc;r</trd>,
    <trd>mature</trd>
  </trdgrp>
  <trd lng="hu">&eacute;rett</trd>
  <trdgrp lng="ja">
    <trd>&#x719F;&#x3057;&#x305F;  [<ind>&#x3053;&#x306A;&#x3057;&#x305F;</ind>]</trd>,
    <trd>&#x6210;&#x719F;&#x3057;&#x305F;  [<ind>&#x305B;&#x3044;&#x3058;&#x3085;&#x304F;&#x3057;&#x305F;</ind>]</trd>,
    <trd>&#x719F;&#x616E;&#x3057;&#x305F;  [<ind>&#x3058;&#x3085;&#x304F;&#x308A;&#x3087;&#x3057;&#x305F;</ind>]</trd>,
    <trd>&#x7DF4;&#x3089;&#x308C;&#x305F;  [<ind>&#x306D;&#x3089;&#x308C;&#x305F;</ind>]</trd>,
    <trd>&#x6E80;&#x671F;&#x306E;  [<ind>&#x307E;&#x3093;&#x304D;&#x306E;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">rijp</trd>
  <trd lng="pl">dojrza&lstroke;y</trd>
  <trd lng="pt">maduro</trd>
  <trdgrp lng="ro">
    <trd>copt</trd>,
    <trd>matur</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_r;&c_je;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_p;&c_je;&c_l;&c_y;&c_j; <klr>(pp frukto)</klr></trd>,
    <trd>&c_v;&c_z;&c_r;&c_o;&c_s;&c_l;&c_y;&c_j; <klr>(pp
    homo)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
  <trd>dospel&yacute;</trd>,
  <trd>vyspel&yacute;</trd>,
  <trd>zrel&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_r;&c_ib;&#x43B;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_t;&c_i;&c_g;&#x43B;&c_i;&c_j;</trd>,
    <trd>&c_s;&#x43F;&c_ib;&#x43B;&c_i;&c_j;</trd>,
    <trd>&c_d;&#x43E;&c_r;&#x43E;&c_s;&#x43B;&c_i;&c_j; (&#x43F;&c_r;&#x43E; &#x43B;&#x44E;&c_d;&c_i;&#x43D;&c_u;)</trd>
  </trdgrp>
</drv>

<drv mrk="matur.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="matur.0eco.KOMUNE">
    <dif>
      Stato de io a&ubreve; iu <tld/>a:
      <ekz>
        unua frukto sur la figarbo en la komenco de &gcirc;ia <tld/>eco
        <fnt>
          <bib>MT</bib>
          <lok>Ho&scirc;ea 9:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        atesto pri <tld/>eco
        <klr>(<ref cel="abitur.0aekzameno">abiturienta ekzameno</ref>;
        pruvo, ke iu estas <tld/>a)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>  
      <ekz>
        konservu bone la donitan semon <klr>[&#x2026;]</klr> &gcirc;is la tempo de <tld/>eco
        <fnt>
          <bib>Fab3</bib>
          <lok>Malnova tomba &scirc;tono</lok>
        </fnt>;
      </ekz>
      <ekz>
        ankora&ubreve; estis plena somero, sed la maturi&gcirc;o kaj <tld/>eco vekis senteton pri
        flavi&gcirc;o kaj disfalo
        <fnt>
          <bib>Tor</bib>
          <lok>IV.</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>eco de nia afero estas jam tute eksterduba
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Oka Kongreso Esperantista en Krakow en la 11a de a&ubreve;gusto 1912</vrk>
        </fnt>;
      </ekz>
      <ekz>
        pluraj infanoj en plena&gcirc;ula kunveno havas nenian signifon por la a&gcirc;a <tld/>eco
        de la a&ubreve;skultantoj, sed &ccirc;eesto de kelkaj elstaraj personoj povas tre altigi
        la tutan nivelon
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
      <trd>&c_s;&c_mol;&c_p;&c_je;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_t;&c_a;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol; 
        <klr>(&c_p;&c_r;&c_a; 
          &c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;&c_a;)</klr></trd>
    </trdgrp>
  <trdgrp lng="br">
      <trd>azvder</trd>,
      <trd>darevder</trd>
    </trdgrp>
  <trd lng="ca">maduresa</trd>
  <trdgrp lng="cs">
  <trd>dosp&ecaron;lost</trd>,
  <trd>vysp&ecaron;lost</trd>,
  <trd>zralost</trd>
  </trdgrp>
  <trd lng="de">Reife</trd>
  <trdgrp lng="en">
      <trd>maturity</trd>,
      <trd>ripeness</trd>
    </trdgrp>
  <trd lng="es">madurez</trd>
  <trd lng="fr">maturit&eacute;</trd>
  <trd lng="hu">&eacute;retts&eacute;g</trd>
  <trdgrp lng="ja">
    <trd>&#x6210;&#x719F;  [<ind>&#x305B;&#x3044;&#x3058;&#x3085;&#x304F;</ind>]</trd>,
    <trd>&#x5186;&#x719F;  [<ind>&#x3048;&#x3093;&#x3058;&#x3085;&#x304F;</ind>]</trd>,
    <trd>&#x6210;&#x5E74;&#x671F;  [<ind>&#x305B;&#x3044;&#x306D;&#x3093;&#x304D;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">rijpheid</trd>
  <trd lng="pl">dojrza&lstroke;o&sacute;&cacute;</trd>
  <trdgrp lng="ru">
      <trd>&c_z;&c_r;&c_je;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_p;&c_je;&c_l;&c_o;&c_s;&c_t;&c_mol; <klr>(pp
      frukto)</klr></trd>
    </trdgrp>
  <trdgrp lng="sk">
  <trd>dospelos&tcaron;</trd>,
  <trd>vyspelos&tcaron;</trd>,
  <trd>zrelos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_r;&c_ib;&#x43B;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&c_s;&c_t;&c_i;&c_g;&#x43B;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&c_s;&#x43F;&c_ib;&#x43B;&c_ib;&c_s;&c_t;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="matur.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="matur.0igi.KOMUNE">
    <dif>
      Igi <tld/>a, konduki al plena disvolvi&gcirc;o:
      <ekz>
        ni devas precipe en nia juneco havi edukistojn,
        kiuj instruas kaj <tld/>igas nin por la a&ubreve;tuna vivo
        <fnt>
          <aut>E. Macheiner</aut>
          <vrk>La kvar sezonoj kaj la homa vivo</vrk>
          <lok>Espero Katolika, 1925, p. 116a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la figarbo jam <tld/>igas siajn fruktetojn
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 2:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen ekfloris la bastono <klr>[&#x2026;]</klr> kaj &gcirc;i elirigis bur&gcirc;onojn kaj donis
        florojn kaj <tld/>igis migdalojn
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 17:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni povis rimarki tiun saman seriozan korecon, kiun &ccirc;e &scirc;ia pli maljuna fratino
        la senpatrine travivita juneco jam longe <tld/>igis
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">ausreifen</trd>
  <trdgrp lng="fr">
    <trd>faire <ind>m&ucirc;rir</ind></trd>,
    <trd>faire <ind>murir</ind></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x719F;&#x6210;&#x3055;&#x305B;&#x308B;  [<ind>&#x3058;&#x3085;&#x304F;&#x305B;&#x3044;&#x3055;&#x305B;&#x308B;</ind>]</trd>,
    <trd>&#x6210;&#x719F;&#x3055;&#x305B;&#x308B;  [<ind>&#x305B;&#x3044;&#x3058;&#x3085;&#x304F;&#x3055;&#x305B;&#x308B;</ind>]</trd>,
    <trd>&#x767A;&#x9054;&#x3055;&#x305B;&#x308B;  [<ind>&#x306F;&#x3063;&#x305F;&#x3064;&#x3055;&#x305B;&#x308B;</ind>]</trd>,
    <trd>&#x7DF4;&#x308A;&#x4E0A;&#x3052;&#x308B;  [<ind>&#x306D;&#x308A;&#x3042;&#x3052;&#x308B;</ind>]</trd>
  </trdgrp>
</drv>

<drv mrk="matur.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="matur.0igxi.KOMUNE">
    <dif>
      I&gcirc;i <tld/>a:
      <ekz>
        tute <tld/>i&gcirc;is <klr>[la]</klr> vinberoj
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 14:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tagoj <tld/>i&gcirc;is por lia suprenakcepto
        <fnt>
          <bib>NT</bib>
          <lok>Luko 9:51</lok>
        </fnt>;
      </ekz>
      <ekz>
        la greno elkreskis, kaj fine <tld/>i&gcirc;is
        <fnt>
          <bib>Rob</bib>
          <lok>Parto III</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia viza&gcirc;o <tld/>i&gcirc;is kaj portas esprimon tute proprekvalitan
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiel <tld/>i&gcirc;is en mi la konscio, ke mi tamen devas ser&ccirc;i laboron, &ccirc;ar la
        nenifarado ne estas longe tolerebla
        <fnt>
          <bib>VaK</bib>
          <lok>Sesa &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        pasos ankora&ubreve; tre kaj tre multe da jaroj,
        anta&ubreve; ol la registaroj trovos la demandon
        de lingvo internacia sufi&ccirc;e <tld/>i&gcirc;inta
        <fnt><bib>EE</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_p;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_a;&c_s;&c_mol;&c_p;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_s;&c_mol;&c_p;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>   
  <trdgrp lng="br">
    <trd>azvi&ntilde;</trd>,
    <trd>darevi&ntilde;</trd>
  </trdgrp>
  <trd lng="ca">madurar</trd>
  <trd lng="de">reifen</trd>   
  <trdgrp lng="en">
    <trd>mature</trd>,
    <trd>ripen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>m&ucirc;rir</trd>,
    <trd>murir</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;rik</trd>,
    <trd>meg&eacute;rik</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x719F;&#x3059;&#x308B;  [<ind>&#x3058;&#x3085;&#x304F;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x6210;&#x719F;&#x3059;&#x308B;  [<ind>&#x305B;&#x3044;&#x3058;&#x3085;&#x304F;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x5341;&#x5206;&#x767A;&#x9054;&#x3059;&#x308B;  [<ind>&#x3058;&#x3085;&#x3046;&#x3076;&#x3093;&#x306F;&#x3063;&#x305F;&#x3064;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x7DF4;&#x308A;&#x4E0A;&#x3052;&#x3089;&#x308C;&#x308B;  [<ind>&#x306D;&#x308A;&#x3042;&#x3052;&#x3089;&#x308C;&#x308B;</ind>]</trd>,
    <trd>&#x6E80;&#x671F;&#x306B;&#x306A;&#x308B;  [<ind>&#x307E;&#x3093;&#x304D;&#x306B;&#x306A;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">rijpen</trd>
  <trdgrp lng="pl">
    <trd>dojrzewa&cacute;</trd>,
    <trd>dorasta&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_z;&c_r;&c_je;&c_t;&c_mol;</trd>
  <trd lng="sv">mogna</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&#x43E;&c_z;&c_r;&c_ib;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_d;&#x43E;&c_s;&c_t;&c_i;&c_g;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_d;&#x43E;&c_s;&#x43F;&c_ib;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43D;&c_a;&c_z;&c_r;&c_ib;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="matur.mol0a">
  <kap>mol<tld/>a</kap>
  <uzo tip="klr">(pri frukto)</uzo>
  <snc mrk="matur.mol0a.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Moli&gcirc;inta kaj duone putri&gcirc;anta pro
      tro<tld/>eco.
    </dif>
  </snc>
