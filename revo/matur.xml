<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>matur</rad>/a
</kap>
<drv mrk="matur.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Atinginta sian plenan kreskadon, forton, kapablon a&ubreve; 
ta&ubreve;gecon:
      <ekz>
        <tld/>a frukto, semo, homo, a&gcirc;o<fnt>Z</fnt>,
      </ekz>
      <ekz>
        spirito, talento.
      </ekz>
    </dif>
    <trd lng="sv">mogen</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plene esplorita, provita, finstudita:
      <ekz>
        <tld/>a projekto, diskutado<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ne<tld/>a demando;
      </ekz>
      <ekz>
        <tld/>e pripensita kaj orde vo&ccirc;donita decido<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>             
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_p;&c_je;&c_l;&c_y;</trd>,
    <trd>&c_s;&c_a;&c_s;&c_mol;&c_p;&c_je;&c_l;&c_y;</trd>,
    <trd>&c_d;&c_a;&c_r;&c_o;&c_s;&c_l;&c_y; <klr>(&c_p;&c_r;&c_a; 
&c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;&c_a;)</klr></trd>,
    <trd>&c_s;&c_t;&c_a;&c_l;&c_y; <klr>(&c_p;&c_r;&c_a; 
&c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;&c_a;)</klr></trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>azv</trd>,
    <trd>darev</trd>
  </trdgrp>
  <trd lng="cs">zral&yacute;</trd>
  <trd lng="de">reif</trd>
  <trd lng="en">mature</trd>
  <trdgrp lng="fr">
    <trd>m&ucirc;r</trd>,
    <trd>mature</trd>
  </trdgrp>
  <trd lng="hu">&eacute;rett</trd>  
  <trd lng="nl">rijp</trd>
  <trd lng="pl">dojrza&lstroke;y</trd>
  <trdgrp lng="ro">
    <trd>copt</trd>,
    <trd>matur</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_r;&c_je;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_p;&c_je;&c_l;&c_y;&c_j; <klr>(pp frukto)</klr></trd>,
    <trd>&c_v;&c_z;&c_r;&c_o;&c_s;&c_l;&c_y;&c_j; <klr>(pp homo)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="matur.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Stato de io a&ubreve; iu <tld/>a;
      <ekz>
        atesto pri <tld/>eco
      </ekz>
      (diplomo donata al lernantoj, kiuj, fininte la gimnazian kursaron,
      sukcese plenumis la koncernan ekzamenon.
    </dif>
    <refgrp tip="sin">
      <ref cel="abitur.0o">abituriento</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_p;&c_je;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol; 
<klr>(&c_p;&c_r;&c_a; 
&c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;&c_a;)</klr></trd>
  </trdgrp>     
  <trdgrp lng="br">
    <trd>azvder</trd>,
    <trd>darevder</trd>
  </trdgrp>                  
  <trd lng="de">Reife</trd>
  <trd lng="fr">maturit&eacute;</trd>
  <trd lng="hu">&eacute;retts&eacute;g</trd>  
  <trd lng="nl">rijpheid</trd>
  <trd lng="pl">dojrza&lstroke;o&sacute;&cacute;</trd>  
  <trdgrp lng="ru">
    <trd>&c_z;&c_r;&c_je;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_je;&c_l;&c_o;&c_s;&c_t;&c_mol; <klr>(pp 
frukto)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="matur.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      I&gcirc;i <tld/>a:
      <ekz>
        <tld/>i&gcirc;anta <klr>(viri&gcirc;anta)</klr> knabo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        lia talento plene <tld/>i&gcirc;is;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la demando ne estas sufi&ccirc;e <tld/>i&gcirc;inta<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_p;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_a;&c_s;&c_mol;&c_p;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_s;&c_mol;&c_p;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>   
  <trdgrp lng="br">
    <trd>azvi&ntilde;</trd>,
    <trd>darevi&ntilde;</trd>
  </trdgrp>
  <trd lng="de">reifen</trd>   
  <trd lng="fr">m&ucirc;rir</trd>
  <trdgrp lng="hu">
    <trd>&eacute;rik</trd>,
    <trd>meg&eacute;rik</trd>
  </trdgrp>
  <trd lng="nl">rijpen</trd>
  <trdgrp lng="pl">
    <trd>dojrzewa&cacute;</trd>,
    <trd>dorasta&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_z;&c_r;&c_je;&c_t;&c_mol;</trd>
  <trd lng="sv">mogna</trd>
</drv>
</art>
