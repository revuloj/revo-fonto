   <ofc>*</ofc>
   <rad>intern</rad>/a
</kap>

<drv mrk="intern.0a">
   <kap><ofc>*</ofc><tld/>a</kap>
   <snc mrk="intern.0a.malekstera">
     <dif>
       <ref tip="sin" cel="en.0a.interna">Ena</ref> de la limoj de objekto, de korpo a&ubreve; de iu
       fermita spaco;
       mal<ref tip="ant" cel="ekster.0a">ekstera</ref>:
      <ekz>
        li eniris en <tld/>an &ccirc;ambron kaj ploris tie
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 43:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        li starigis la kerubojn meze de la <tld/>a parto de la domo
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 6:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        li konstruis la <tld/>an korton el tri vicoj da &ccirc;irka&ubreve;hakitaj &scirc;tonoj
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 6:36</lok>
        </fnt>;
      </ekz>
      <ekz>
        al la flanko <tld/>a la ringa montaro estis tute kruta kaj formis grandegan
        rondvalon
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a flanko de la urba murego
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>an flankon <klr>[de la felo]</klr> oni turnis eksteren
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a flanko de la kovrilo estis ornamita per pejza&gcirc;o
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la patrino zorgis mem pri la <tld/>aj aferoj de la domo kaj la patro pri la
        eksteraj
        <fnt>
          <bib>Fab3</bib>
        </fnt>;
      </ekz>
      <ekz>
        en &ccirc;iun el la urnoj metis iun <tld/>an organon de l' mortinto
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>.
      </ekz>
     </dif>
     <trdgrp lng="de">
       <trd>innen</trd>,
       <trd>inwendig</trd>,
       <trd>drinnen</trd>,
       <trd>innerhalb</trd>,
       <trd>intern</trd>
     </trdgrp>
     <trd lng="en">internal</trd>
     <trd lng="es">interno</trd>
     <trdgrp lng="nl">
       <trd>intern</trd>,
       <trd>binnen-</trd>
     </trdgrp>
   </snc>
   <snc mrk="intern.0a.enlanda">
     <uzo tip="fak">POL</uzo>
     <dif>
       Ena de la limoj de lando, kontraste al la eksterlando:
      <ekz>
        Ministerio pri <tld lit="I"/>aj <klr>(enlandaj)</klr> Aferoj
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a milito kaj <tld/>aj konfliktoj <klr>[de Srilanko]</klr>
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="https://tekstaro.com/t?nomo=artikoloj-el-monato">Brito elmigras al tropika
          paradizo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;tata terorismo <tld/>a kaj ekstera
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato1/monato-9503-el-mia-vidpunkto.txt">El Mia
          Vidpunkto</url></vrk>
          <lok>95/03</lok>
        </fnt>.
      </ekz>
     </dif>
     <trdgrp lng="de">
       <trd>inl&auml;ndisch</trd>,
       <trd>binnenl&auml;ndisch</trd>,
       <trd>intern</trd>
     </trdgrp>
     <trdgrp lng="en">
       <trd>internal</trd>,
       <trd>domestic</trd>
     </trdgrp>
     <trd lng="nl">binnenlands</trd>
  </snc>
  <snc mrk="intern.0a.mensa">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Koncernanta la <ref cel="spirit.0a">spirita</ref>jn, moralajn ecojn, kontraste kun la
      korpaj, materiaj:
      <ekz>
        kvankam nia ekstera homo kaduki&gcirc;as, tamen nia <tld/>a homo renovi&gcirc;as tagon post
        tago
        <fnt>
          <bib>NT</bib>
          <lok>II. Korintanoj 4:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj okuloj spegulis &scirc;ian <tld/>an 
        <klr>(<ref cel="kasx.0i.de_okulo">ka&scirc;itan</ref>, 
        <ref cel="latent.0a.FIL">latentan</ref>)</klr>
        amon
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a trankvileco
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova strata lanterno</lok>
        </fnt>;
      </ekz>
      <ekz>
        multe elna&gcirc;is en &gcirc;ia memoro <klr>[&#8230;]</klr> kvaza&ubreve; <tld/>a vo&ccirc;o dirus al &gcirc;i:
        anka&ubreve; min oni rememoros
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova strata lanterno</lok>
        </fnt>!
      </ekz>
      <ekz>
        mediti pri sia propra <tld/>a esenco
        <fnt>
          <bib>Fab3</bib>
          <lok>Lino</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ekkonis mian <tld/>an naturon, la parencecon kiun mi havas kun la poezio
        <fnt>
          <bib>Fab2</bib>
          <lok>La ombro</lok>
        </fnt>;
      </ekz>
      <ekz>
        post momento de <tld/>a batalo li diris la&ubreve;te <klr>[&#8230;]</klr>
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni devas anta&ubreve; &ccirc;io bone klarigi al ni la <tld/>an ideon de la Esperantismo
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Dua Kongreso Esperantista en Gen&egrave;ve en la 28a de a&ubreve;gusto 1906</vrk>
        </fnt>.
      </ekz>
     </dif>
     <trdgrp lng="de">
       <trd>innerlich</trd>,
       <trd>intern</trd>
     </trdgrp>
     <trd lng="en">inner</trd>
     <trd lng="nl">intern</trd>
   </snc>
   <snc mrk="intern.0a.punkto">
     <uzo tip="fak">MAT</uzo>
     <fnt><bib>EKV</bib>, <lok>&para;178</lok></fnt>
     <dif>
       <klr>(p.p. <ref cel="punkt.0o.spaco">punkto</ref> en
       <ref cel="spac.topologia0o">topologia spaco</ref>, rilate al
       subaro <frm><g>A</g></frm>)</klr>
       Tia, ke <frm><g>A</g></frm> inkluzivas
       <ref cel="cxirka.0ajxo.topologio">&ccirc;irka&ubreve;a&jcirc;on</ref>
       de &gcirc;i:
      <ekz>
        cirklo estas kurba linio, kies &ccirc;iuj punktoj estas en egala distanco al <tld/>a
        punkto, nomata centro
        <fnt>
          <vrk><url ref="https://litvek.com/br/431833?p=16">Vortaro de Esperanto 1910</url></vrk>
        </fnt>.
      </ekz>
     </dif>
     <trd lng="de">inner<klr tip="ind"> (-er Punkt)</klr></trd>
     <trd lng="en">inner</trd>
     <trd lng="fr"><klr tip="ind">(point) </klr>int&eacute;rieur</trd>
     <trd lng="pl"><klr tip="ind">(punkt) </klr>wewn&eogonek;trzny</trd>
     <trd lng="ru">&c_v;&c_n;&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_ja;&c_ja;<klr tip="amb">
     (&c_t;&c_o;&c_ch;&c_k;&c_a;)</klr></trd>
   </snc>
  <trdgrp lng="cs">
    <trd>intern&iacute;</trd>,
    <trd>vnit&rcaron;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="fr">
     <trd>interne<klr tip="ind"> (adj.)</klr></trd>,
     <trd>int&eacute;rieur<klr tip="ind"> (adj.)</klr></trd>
   </trdgrp>
  <trdgrp lng="hu">
     <trd>bels&odblac;</trd>,
     <trd>bens&odblac;</trd>,
     <trd>bel-</trd>
   </trdgrp>
  <trd lng="pl">wewn&eogonek;trzny</trd>
  <trdgrp lng="pt">
    <trd>interno<klr tip="amb"> (adj.)</klr></trd>,
    <trd>interior<klr tip="amb"> (adj.)</klr></trd>
   </trdgrp>
  <trd lng="ru">&c_v;&c_n;&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_i;&c_j;</trd>
  <trd lng="sk">vn&uacute;torn&yacute;</trd>
  <trd lng="tok">insa</trd>
  <trd lng="uk">&c_v;&#x43D;&c_u;&c_t;&c_r;&c_ib;&c_sh;&#x43D;&c_ib;&c_j;</trd>
</drv>

<drv mrk="intern.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      En iu koncerna objekto:
      <ekz>
        mi staras ekster la domo, kaj li estas <tld/>e
        <fnt>
          <bib>F</bib>
          <lok>&FE; 26</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pordo estis ne tute fermita, kaj profunde el <tld/>e sonis malla&ubreve;ta kaj
        agrabla muziko
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> rigardis <tld/>en tra fendo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la arkeo<klr>[n&#8230;]</klr> &scirc;miru <klr>[&#8230;]</klr> per pe&ccirc;o <tld/>e kaj ekstere
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 6:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekstere honoro, <tld/>e doloro
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="intern.0e.spirite">
    <dif>
      En sia spirito, animo, pensado ktp:
      <ekz>
        <nom>Sara</nom> ekridis <tld/>e
        <klr>(amuzi&gcirc;is sen tro montri tion)</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 18:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj okuloj parolis pli <tld/>e kaj pli profunde al la koro
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        jen estas la tempo por agi, diris <nom>Mathilde</nom> <tld/>e kaj pripensadis, kiel &scirc;i mem
        povus helpi
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        tio &ccirc;i estas la pordo de la domo de Dio, respondis al li vo&ccirc;o el <tld/>e, nur
        sanktuloj havas la rajton enveni tien
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        fajron sentas mi <tld/>e, vivi anka&ubreve; mi deziras
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        granda kranio, sed <tld/>e nenio
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="sin" cel="en.0e">ene</ref>
  <trd lng="bo">&t_na;&t_nga;&t_tsheg;&t_la;&t_tsheg;</trd>
  <trd lng="de">innen</trd>
   <trdgrp lng="en">
     <trd>in</trd>,
     <trd>internally</trd>
   </trdgrp>
   <trd lng="es">internamente</trd>
   <trdgrp lng="fr">
     <trd>&agrave; l'<ind>int&eacute;rieur</ind></trd>,
     <trd>int&eacute;rieurement</trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>bel&uuml;l</trd>,
     <trd>bent</trd>
   </trdgrp>
   <trd lng="nl">van <ind>binnen</ind></trd>
   <trdgrp lng="pl">
     <trd>wewn&aogonek;trz</trd>,
     <trd>w &sacute;rodku</trd>
   </trdgrp>
   <trd lng="pt">internamente</trd>
   <trd lng="ru">&c_v;&c_n;&c_u;&c_t;&c_r;&c_i;</trd>
  <trd lng="uk">&c_u;&c_s;&c_je;&c_r;&c_je;&c_d;&c_i;&#x43D;&c_ib;</trd>
</drv>

<drv mrk="intern.0ede">
  <kap><tld/>e de</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="en.0.kie">En</ref>, inter la limoj de, sub
      la ekstera&jcirc;o de:
      <ekz>
        <klr>[li]</klr> donis al &ccirc;iuj en la manojn <klr>[&#8230;]</klr> kru&ccirc;ojn, kun tor&ccirc;oj
        <tld/>e de la kru&ccirc;oj
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 7:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        Li vokis Moseon en la sepa tago el <tld/>e de la nubo
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 24:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la cedroj <tld/>e de la domo estis skulptitaj tuberoj kaj floroj
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 6:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        fiereco kaj tima zorgo malfacile kaj kruele batalis <tld/>e de &scirc;i
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trd lng="cs">uvnit&rcaron;</trd>
  <trdgrp lng="en">
     <trd>in</trd>,
     <trd>inside</trd>
   </trdgrp>
  <trd lng="fr">&agrave; l'int&eacute;rieur de</trd>
  <trdgrp lng="hu">
     <trd><klr>vmin</klr> bel&uuml;l</trd>,
     <trd><klr>vmi</klr> belsej&eacute;ben</trd>
   </trdgrp>
  <trdgrp lng="pl">
     <trd>wewn&aogonek;trz</trd>,
     <trd>w &sacute;rodku</trd>
   </trdgrp>
  <trd lng="pt">dentro de</trd>
  <trd lng="ru">&c_v;&c_n;&c_u;&c_t;&c_r;&c_i;</trd>
  <trd lng="sk">vn&uacute;tri</trd>
</drv>

<drv mrk="intern.0o">
  <kap><tld/>o</kap>
  <snc mrk="intern.0o.parto">
    <dif>
      La <tld/>a parto, spaco:
      <ekz>
        la ekstero de tiu &ccirc;i homo estas pli bona, ol lia <tld/>o
        <fnt>
          <bib>F</bib>
          <lok>&FE; 31</lok>
        </fnt>;
      </ekz>
      <ekz>
        la infanoj interpu&scirc;i&gcirc;is en &scirc;ia <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 25:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pastroj eniris en la <tld/>on de la domo
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 29:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio pruvas <klr>[&#8230;]</klr> ke en la <tld/>o de la palaco ni havas perfidulojn
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ian spiriton kaptis mallumaj furioj, elkovi&gcirc;intaj sur la fundo de &scirc;ia
        ribeli&gcirc;inta <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        amo por li kaj memoro pri li staras en mia <tld/>o kiel altaroj
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
     <trdgrp lng="fr">
       <trd>int&eacute;rieur<klr tip="ind"> (subst.)</klr></trd>,
       <trd>dedans<klr tip="ind"> (subst.)</klr></trd>
     </trdgrp>
     <trd lng="pl">wn&eogonek;trze</trd>
     <trd lng="pt">interior</trd>
  </snc>
  <snc mrk="intern.0o.topologio">
      <uzo tip="fak">MAT</uzo>
      <fnt><bib>EKV</bib>, <lok>&para;178</lok></fnt>
      <ref tip="dif" cel="ferm.mal0ajxo.MAT">malferma&jcirc;o.</ref>
       <trdgrp lng="de">
         <trd>Inneres</trd>,
         <trd>offener <ind>Kern</ind></trd>
       </trdgrp>
       <trd lng="fr">int&eacute;rieur<klr tip="ind"> (d'un
  sous-ensemble)</klr></trd>
      <trd lng="hu">ny&iacute;lt <ind>mag</ind></trd>
      <trd lng="pl">wn&eogonek;trze<klr tip="ind"> (zbioru)</klr></trd>
      <trd lng="pt">interior</trd>
      <trdgrp lng="ru">
        <trd>&c_v;&c_n;&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
        <trd>&c_o;&c_t;&c_k;&c_r;&c_y;&c_t;&c_o;&c_je;
        <ind>&c_ja;&c_d;&c_r;&c_o;</ind></trd>
      </trdgrp>
  </snc>   
  <trdgrp lng="cs">
    <trd>l&uring;no</trd>,
    <trd>nitro</trd>,
    <trd>vnit&rcaron;ek</trd>
  </trdgrp>
  <trdgrp lng="de">
       <trd>Inneres</trd>,
       <trd>offener <ind>Kern</ind></trd>
     </trdgrp>
  <trd lng="en">interior</trd>
  <trd lng="fr">int&eacute;rieur<klr tip="ind"> (d'un
sous-ensemble)</klr></trd>
  <trdgrp lng="hu">
     <trd>bels&odblac;</trd>,
     <trd>belseje</trd>
   </trdgrp><trd lng="hu">ny&iacute;lt <ind>mag</ind></trd>
  <trdgrp lng="nl">
     <trd>inwendige</trd>,
     <trd>binnenste</trd>
   </trdgrp>
  <trd lng="pl">wn&eogonek;trze<klr tip="ind"> (zbioru)</klr></trd>
  <trd lng="pt">interior</trd>
  <trdgrp lng="ru">
       <trd>&c_v;&c_n;&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
       <trd>&c_o;&c_t;&c_k;&c_r;&c_y;&c_t;&c_o;&c_je; <ind>&c_ja;&c_d;&c_r;&c_o;</ind></trd>
     </trdgrp><trdgrp lng="ru">
     <trd>&c_v;&c_n;&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
     <trd>&c_v;&c_n;&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_ja;&c_ja; &c_ch;&c_a;&c_s;&c_t;&c_mol;</trd>
   </trdgrp>
  <trd lng="sk">vn&uacute;traj&scaron;ok</trd>
  <trd lng="tok">insa</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&#x43D;&c_u;&c_t;&c_r;&c_ib;&c_sh;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&c_v;&#x43D;&c_u;&c_t;&c_r;&c_ib;&c_sh;&#x43D;&#x44F; &c_ch;&c_a;&c_s;&c_t;&c_i;&#x43D;&c_a;</trd>,
    <trd>&c_ib;&#x43D;&c_t;&c_je;&c_r;&#8217;&c_jeu;&c_r;</trd>,
    <trd>&c_v;&#x43D;&c_u;&c_t;&c_r;&c_ib;&c_sh;&#x43D;&c_ib;&c_j; &c_s;&c_v;&c_ib;&c_t;</trd>,
    <trd>&c_d;&c_u;&c_sh;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="intern.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Tio, kio estas enhavata en la <tld/>o de io:
      <ekz>
        <klr>[&scirc;i]</klr> parolis kun li pri <klr>[&#8230;]</klr> l' <tld/>a&jcirc;o kaj ekstera&jcirc;o de
        l' homo
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> diras, ke la <tld/>a&jcirc;o de la tero konsistas el fajra fandita
        maso
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a&jcirc;o de la dometo konsistis el malgranda kaverno, kies tri kvaronojn
        okupis granda rusa forno
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;ipo ku&scirc;i&gcirc;is sur flanko kaj la akvo eniris en la <tld/>a&jcirc;on de la &scirc;ipo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        nur tiu, kiu eliros el via <tld/>a&jcirc;o, estos via heredanto
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 15:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        man&gcirc;u &gcirc;in <klr>[&#8230;]</klr> rostitan sur fajro kun &gcirc;iaj kapo, 
        kruroj, kaj <tld/>a&jcirc;oj <klr>(<ref cel="organ.0o.BIO">organoj</ref>)</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 12:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        Kiu do vi estas, // Vi, kies vo&ccirc;o kruelege turnas // La plej profundan mian
        <tld/>a&jcirc;on
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>?
      </ekz>
      <ekz>
        <klr>[ili]</klr> el&scirc;utas el kofroj ilian <tld/>a&jcirc;on
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj fratinoj elna&gcirc;is el la <tld/>a&jcirc;o de la maro
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekip.0ajxo">ekipa&jcirc;o</ref>,
      <ref cel="hav.en0o">enhavo</ref>,
      <ref cel="intest.0o">intesto</ref>,
      <ref cel="kern.0o.interno">kerno<sncref/></ref>
    </refgrp>
  </snc>
   <trdgrp lng="de">
    <trd>Inneres</trd>,
    <trd>Innerei</trd>,
    <trd>Einrichtung</trd>
  </trdgrp>
  <trd lng="en">interior</trd>
   <trd lng="es">entra&ntilde;as</trd>
   <trdgrp lng="fr">
     <trd>int&eacute;rieur<klr tip="ind"> (subst.)</klr></trd>,
     <trd>entrailles</trd>,
     <trd>dedans<klr tip="ind"> (subst.)</klr></trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>bels&odblac;s&eacute;g</trd>,
     <trd>bennlev&odblac;s&eacute;g</trd>,
     <trd>bels&odblac; tartalom</trd>,
   </trdgrp>
   <trd lng="nl">binnenste</trd>
   <trdgrp lng="pl">
     <trd>wn&eogonek;trzno&sacute;ci</trd>,
     <trd>wn&eogonek;trze</trd>
   </trdgrp>
   <trdgrp lng="pt">
    <trd>entranhas</trd>,
    <trd>&acirc;mago</trd>
   </trdgrp>
   <trd lng="ru">&c_v;&c_n;&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&#x43D;&c_u;&c_t;&c_r;&c_ib;&c_sh;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&c_v;&#x43D;&c_u;&c_t;&c_r;&c_ib;&c_sh;&#x43D;&#x44F; &c_ch;&c_a;&c_s;&c_t;&c_i;&#x43D;&c_a;</trd>,
    <trd>&#x43D;&c_u;&c_t;&c_r;&#x43E;</trd>,
    <trd>&#x43D;&c_u;&c_t;&c_r;&#x43E;&c_shch;&c_ib;</trd>
  </trdgrp>
</drv>

<drv mrk="intern.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="intern.0igi.KOMUNE">
    <dif>
      Meti en la <tld/>on de, igi <tld/>a:
      <ekz>
        kiam infanoj dekomence <tld/>igas la&ubreve;le&gcirc;emon en sin <klr>[&#8230;]</klr> ili kreskinte
        rektigas kio ajn estis anta&ubreve;e renversita en la civito
        <fnt>
          <bib>Rsp</bib>
        </fnt>;
      </ekz>
      <ekz>
        tute <tld/>igita devoteco en la praktika &ccirc;iutaga vivo
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="&Monato;/2009/009781.php">La sola nederlanddevena
          papo</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi tiom <tld/>igis la ritmon de la lecionoj, ke mi preska&ubreve; povis senrigarde
        diveni la minuton
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2012/010654.php">Horobato, korobato</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="en.0igi">enigi</ref>,
       <ref cel="gravur.0i.FIG">gravuri<sncref/></ref>,
       <ref cel="parker.0igi">parkerigi</ref>,
       <ref cel="regist.0i.FIG">registri<sncref/></ref>,
       <ref cel="sorb.0i.FIG">sorbi<sncref/></ref>
    </refgrp>
     <trd lng="de">verinnerlichen</trd>
     <trdgrp lng="en"><trd>insert</trd>, <trd>internalize</trd></trdgrp>
     <trd lng="fr">faire <ind>entrer</ind></trd>
     <trdgrp lng="pl">
       <trd>wk&lstroke;ada&cacute;</trd>,
       <trd>umieszcza&cacute; <klr tip="amb">(we wn&eogonek;trzu)</klr></trd>
     </trdgrp>
     <trd lng="ru">&c_v;&c_v;&c_je;&c_s;&c_t;&c_i;</trd>
  </snc>
  <snc mrk="intern.0igi.speciale">
    <uzo tip="fak">PED</uzo><uzo tip="fak">MIL</uzo><uzo tip="fak">POL</uzo>
    <dif>
      Meti en fermitan institucion:
      <ekz>
        mi konjektis, ke tie minimume estas mil infanoj <tld/>igitaj
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        atentindan helpon donis la servo al <ind><tld/>igito</ind>j en Francio: unue al la
        ekzilitoj el Hispanio kaj poste al la forpelitaj Judoj el Germanio kaj
        Pollando
        <fnt>
          <bib>EeP</bib>
        </fnt>;
        <trd lng="de">Internierter</trd>
      </ekz>
      <ekz>
        via hejmlando estas kontestata, kaj tial ni devas meti vin en <ind><tld/>igejo</ind>n
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
        <trd lng="de">Internierungslager</trd>
      </ekz>
      <ekz>
        <klr>[ili]</klr> estis cetere fermitaj tute aparte, kiun lokon mi ne scias kiel
        nomi: <ind><tld/>iga lagero</ind>, a&ubreve; frenezulejo, el kie ili neniam rajtis elveni al la
        homa publika vivo
        <fnt>
          <bib>VaK</bib>
        </fnt>;
        <trd lng="de">Internierungslager</trd>
        <trd lng="en">internment camp</trd>
      </ekz>
      <ekz>
        post 1918 Sovetio instalis koncentrejojn por <tld/>igi kontra&ubreve;ulojn de la tiama
        re&gcirc;imo
        <fnt>
          <bib>Monato</bib>
          <aut>Josef Maria Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2003/007936.html">Jubileo hontinda</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="prizon.en0igi">enprizonigi</ref>,
      <ref cel="liber.mal0igi">malliberigi</ref>,
      <ref cel="matrik.0i">matrikuli</ref>
    </refgrp>
    <trd lng="de">internieren</trd>
    <trd lng="en">confine</trd>
     <trd lng="fr">interner</trd>
     <trd lng="pl">internowa&cacute;</trd>
     <trd lng="ru">&c_i;&c_n;&c_t;&c_je;&c_r;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
   </snc>
  <trd lng="cs">internovat</trd>
  <trd lng="es">internar</trd>
  <trdgrp lng="hu">
     <trd>bels&odblac;v&eacute; tesz</trd>,
     <trd>intern&aacute;l</trd>,
     <trd>saj&aacute;tj&aacute;v&aacute; tesz</trd>,
   </trdgrp>
  <trd lng="nl">interneren</trd>
  <trd lng="pt">internar</trd>
  <trd lng="sk">internova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_v;&#x43E;&c_d;&c_i;&c_t;&c_i; &c_v;&c_s;&c_je;&c_r;&c_je;&c_d;&c_i;&#x43D;&c_u;</trd>,
    <trd>&c_v;&#x43A;&#x43B;&c_a;&c_d;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&#x43A;&#x43B;&c_a;&c_d;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43E;&c_s;&c_je;&#x43B;&#x44F;&c_t;&c_i;</trd>,
    <trd>&#x43F;&#x43E;&#x43C;&c_ib;&c_shch;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_ib;&#x43D;&c_t;&c_je;&c_r;&#x43D;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="intern.0ulo">
   <kap><tld/>ulo</kap>
  <snc mrk="intern.0ulo.ano">
    <dif>
      Ano de la propra rondo, socio ktp:
      <ekz>
        &ccirc;u vi ne ju&gcirc;as la <tld/>ulojn, dum la eksterulojn ju&gcirc;as Dio? forigu el inter vi
        la malvirtulon
        <fnt>
          <bib>NT</bib>
          <lok>I. Korintanoj 5:12</lok>
        </fnt>;
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Interner</trd>,
      <trd>Eigener</trd>
    </trdgrp>
  </snc>
  <snc mrk="intern.0ulo.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      <tld lit="I"/>a lernanto, kiu lo&gcirc;as kune kun aliaj lernantoj en la koncerna institucio:
      <ekz>
        mi eniris <klr>[&#8230;]</klr> en kolegion, kiel <tld/>ulo
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        pli da lernejanoj signifos, ke necesos novaj dormejoj por <tld/>uloj
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2010/010124.php">Revo realigita</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="cs">obyvatel intern&aacute;tu</trd>
    <trd lng="de">Internatssch&uuml;ler</trd>
    <trd lng="en">boarder</trd>
    <trd lng="es">interno</trd>
    <trd lng="fr">interne<klr tip="ind"> (&eacute;l&egrave;ve)</klr></trd>
    <trdgrp lng="hu">
      <trd>bentlak&oacute;</trd>,
      <trd>internista</trd>,
      <trd>koll&eacute;gista</trd>,
    </trdgrp>
    <trd lng="nl">interne leerling</trd>
    <trdgrp lng="pl">
      <trd>ucze&nacute;<klr tip="amb"> (mieszkaniec internatu)</klr></trd>,
      <trd>wychowanek<klr tip="amb"> (mieszkaniec internatu)</klr></trd>
    </trdgrp>
    <trd lng="pt">interno<klr tip="amb"> (pessoa)</klr></trd>
    <trd lng="ru">&c_i;&c_n;&c_t;&c_je;&c_r;&c_n;</trd>
    <trd lng="sk">obyvate&lcaron; intern&aacute;tu</trd>
  </snc>
</drv>

<drv mrk="intern.0ulejo">
  <kap><tld/>ulejo</kap>
  <snc mrk="intern.0ulejo.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      <ref tip="super" cel="lern.0ejo.loko">Lernejo</ref>, kie lernantoj (la <tld/>uloj) lo&gcirc;as kune:
      <ekz>
        la nova internulejo povis malfermi&gcirc;i
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="https://tekstaro.com/t?nomo=artikoloj-el-monato">Alia tempo, aliaj
          moroj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ekster la urbocentro, trovi&gcirc;as Ekonomia Mezlernejo, en kies internulejo okazos
        la programo
        <fnt>
          <bib>LOdE</bib>
          <vrk><url ref="https://tekstaro.com/t?nomo=ondo-de-esperanto">Bonvenon al
          Olsztyn!</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">domov ml&aacute;de&zcaron;e</trd>
  <trd lng="de">Internat</trd>
  <trd lng="en">boarding school</trd>
  <trd lng="fr">internat, pensionnat</trd>
  <trdgrp lng="hu">
    <trd>koll&eacute;gium</trd>,
    <trd>intern&aacute;tus</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>internat</trd>,
    <trd>bursa<klr tip="amb"> (daw.)</klr></trd>
  </trdgrp>
  <trd lng="pt">internato</trd>
  <trd lng="sk">intern&aacute;t <klr>(chlap&ccaron;ensk&yacute;)</klr></trd>
  <trd lng="uk">&c_ib;&#x43D;&c_t;&c_je;&c_r;&#x43D;&c_a;&c_t;</trd>
</drv>
