<drv mrk="reg.mem0ado">
  <kap>mem<tld/>ado, <var><kap>mem<tld/>eco</kap></var>,
  <var><kap>mem<tld/>o</kap></var></kap>
  <snc mrk="reg.mem0ado.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      La <ref tip="super" cel="rajt.0o">rajto</ref> <tld/>i sin, fari proprajn decidojn kaj
      le&gcirc;ojn pri sia vivo:
      <ekz>
        li parolis pri popola mem<tld/>ado kaj popola memklerigo
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>;
      </ekz>
      <ekz>
        jen urbo, kie kolekti&gcirc;is anta&ubreve; kvincent jaroj la unua
        parlamento kaj metis fundamenton por tutpopola
        mem<tld/>ado
        <fnt>
          <aut>S. Engholm</aut>, <vrk>Vivo vokas</vrk>, <lok>1946</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni tiamaniere fondus veran federalan registaron, kiu tamen respektus la
        mem<tld/>on de la <esc>dudekdu</esc> apartaj &scirc;tatoj
        <fnt>
          <bib>FSp</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="auxton.0eco">a&ubreve;tonomeco</ref>,
      <ref cel="pend.sende0eco.POL">sendependeco</ref>
    </refgrp>
    <trd lng="be">&c_s;&c_a;&c_m;&c_a;&c_k;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
    <trdgrp lng="de">
      <trd>Selbstverwaltung</trd>,
      <trd>Selbstbestimmung</trd>,
      <trd>Autonomie</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd><baz>perintah</baz>pemerintahan sendiri</trd>,
      <trd>otonomi</trd>,
      <trd>swatantra</trd>,
      <trd>berdikari</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_a;&c_m;&c_o;&c_u;&c_p;&c_r;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="reg.mem0ado.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      La kapablo funkcii sen ekstera <tld/>ado, <ref cel="auxtom.0e">a&ubreve;tomate</ref>:
      <ekz>
        la nuna esplorado male celas da&ubreve;ran mem<tld/>econ,
        kapablon por la roboto konstrui mem planon
        <fnt>
          <aut>F. Maurelli</aut>,
          <vrk><url ref="&UEA;dokumentoj/IKU/2012/Maurelli.html">
          Subakva robotiko (resumo)</url></vrk>,
          <lok>IKU-2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_a;&c_m;&c_a;&c_k;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_a;&c_w;&c_t;&c_a;&c_n;&c_o;&c_m;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Selbststeuerung</trd>,
      <trd>Automatik</trd>
    </trdgrp>
    <trd lng="es">autonom&iacute;a</trd>
    <trdgrp lng="id">
      <trd>otomatisasi</trd>,
      <trd><baz>otomatis</baz>pengotomatisan</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_a;&c_m;&c_o;&c_u;&c_p;&c_r;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="reg.mem0ado.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      <ref tip="dif" cel="reg.sin0ado.PSI">Sin<tld/>ado</ref>:
      <ekz>
        kaj ne sole li reakiris la mem<tld/>adon, sed e&ccirc; li
        eksentis sin senfine supera, ol &ccirc;io vivanta
        <fnt><bib>Far3</bib>, <lok>&ccirc;ap. 14a.</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;u kolerema? ne kolerema, sed ... eksplodema, &gcirc;is perdo de mem<tld/>o
        <fnt>
          <bib>Lanti</bib>
        </fnt>;
      </ekz><ekz>
        mi perdas mian mem<tld/>adon, freneza kolero donas al
        mi la forton malfermi la pordon de l' &scirc;ranko, kaj mi
        batas &scirc;in per la pugnoj, per la piedoj
        <fnt>
          <aut>H. Garc&iacute;a</aut>, <vrk>Vanesa</vrk>, <bib>Monato</bib>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      <ctl>Mem</ctl> akcentas a&ubreve;tonomecon de agado, maleston de ekstera
      (mal)helpo; <ctl>sin</ctl> akcentas ke la objekto de la ago
      estas la subjekto. Tial por la signifo psi&hcirc;ologia pli konvenas
      &leftquot;sin<tld/>ado&rightquot;, kaj pri la sencoj politika kaj te&hcirc;nika,
      &leftquot;mem<tld/>ado&rightquot;. <aut>Sergio Pokrovskij</aut>
    </rim>
    <trd lng="be">&c_s;&c_a;&c_m;&c_a;&c_v;&c_a;&c_l;&c_o;&c_d;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
    <trdgrp lng="de">
      <trd>Selbstbeherrschung</trd>,
      <trd>Selbstkontrolle</trd>
    </trdgrp>
    <trd lng="es">autocontrol</trd>
    <trd lng="fr">maitrise de soi</trd>
    <trd lng="hu">&ouml;nuralom</trd>
    <trd lng="id"><ind>kontrol</ind> diri</trd>
    <trd lng="nl">zelfbeheersing</trd>
    <trdgrp lng="pl">
      <trd>panowanie nad sob&aogonek;</trd>
      <trd>autonomia</trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>autocontrol</trd>
      <trd>autonomie</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_a;&c_m;&c_o;&c_o;&c_b;&c_l;&c_a;&c_d;&c_a;&c_n;&c_i;&c_je;</trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x81EA;&#x6CBB;  <pr>&#12376;&#12385;</pr></trd>,
    <trd>&#x81EA;&#21046;  <pr>&#12376;&#x305B;&#12356;</pr></trd>,
    <trd>&#x81EA;&#21046;&#x5FC3;  <pr>&#12376;&#x305B;&#12356;&#12375;&#12435;</pr></trd>,
    <trd>&#x81EA;&#x52D5;&#21046;&#x5FA1;  <pr>&#12376;&#12393;&#12358;&#x305B;&#12356;&#x304E;&#12423;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_a;&c_m;&c_o;&c_v;&c_r;&c_ja;&c_d;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_a;&c_v;&c_t;&c_o;&c_n;&c_o;&c_m;&c_ib;&c_ja;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_o;&c_v;&c_l;&c_a;&c_d;&c_a;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="reg.mis0i">
  <kap>
    mis<tld/>i
  </kap>
  <snc>
    <dif>
      <tld lit="R"/>i malpropre, malbone:
      <ekz>
        la valonaj politikistoj mis<tld/>is, ili disperdis la ri&ccirc;a&jcirc;ojn de &ccirc;iuj, ili ne
        faris la bonajn decidojn pri la ekonomia bonstato de Belgio
        <fnt>
          <bib>Monato</bib>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="&Monato;/2007/009338.php">Separismo ne, egalrajteco jes</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        sekvis serio da nelongeda&ubreve;raj mis<tld/>antaj sud-vjetnamaj administracioj
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2005/008073.php">Popolo suferinta pro agresemo de
          aliaj</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol; &c_d;&c_r;&c_e;&c_n;&c_n;&c_a;, &c_n;&c_ja;&c_p;&c_r;&c_a;&c_v;&c_ib;&c_l;&c_mol;&c_n;&c_a;</trd>
  <trd lng="de">schlecht <ind>regieren</ind></trd>
  <trdgrp lng="en">
    <trd>misrule</trd>,
    <trd>misgovern</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>salah</ind> pimpin</trd>,
    <trd><ind>salah</ind> atur</trd>,
    <trd><ind>salah</ind> perintah</trd>
  </trdgrp>
  <trd lng="pl">&zacute;le rz&aogonek;dzi&cacute;</trd>
  <trd lng="ro">proast&abreve; guvernare</trd>
</drv>

<drv mrk="reg.sin0ado">
  <kap>sin<tld/>ado,
  <var><kap>sin<tld/>o</kap></var></kap>
  <snc mrk="reg.sin0ado.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      Kapablo <tld/>i sin, <tld/>i siajn emociojn, kaj rezulta
      <ref tip="vid" cel="trankv.0o.anima">trankvilo</ref>:
      <ekz>
        sinjoro Pikviko vidigis tiujn perfektan malpanikon kaj
        sin<tld/>on, kiuj estas la nemalhaveblaj akompanantoj de menso
        grandioza
        <fnt>
          <aut>Ch. Dickens, tr. W. Auld</aut>:
          <vrk><url ref="ht://esperanto-ondo.ru/counter/counter.php?libro=21">La
          postlasitaj aferoj de la Klubo Pikvika</url></vrk>.
          <lok>&Ccirc;ap. 4&#xAA;.</lok>
        </fnt>;
      </ekz><ekz>
         kuni&gcirc;u denove, por ke Satano vin ne tentu pro manko de
         sin<tld/>ado
         <fnt><bib>NT</bib>, <lok>&Ko1; 7:5</lok></fnt>.
       </ekz>
    </dif>
    <trd lng="be">&c_s;&c_a;&c_m;&c_a;&c_v;&c_a;&c_l;&c_o;&c_d;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
    <trd lng="es">autocontrol</trd>
    <trd lng="fr">maitrise de soi</trd>
    <trd lng="hu">&ouml;nuralom</trd>
    <trd lng="nl">zelfbeheersing</trd>
    <trd lng="ru">&c_s;&c_a;&c_m;&c_o;&c_o;&c_b;&c_l;&c_a;&c_d;&c_a;&c_n;&c_i;&c_je;</trd>
  </snc>
<trd lng="de">Selbstbeherrschung</trd>
  <trd lng="id"><ind>kontrol</ind> diri</trd>
  <trdgrp lng="ja">
    <trd>&#x81EA;&#21046;  <pr>&#12376;&#x305B;&#12356;</pr></trd>,
    <trd>&#x81EA;&#21046;&#x5FC3;  <pr>&#12376;&#x305B;&#12356;&#12375;&#12435;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_a;&c_m;&c_o;&c_v;&c_r;&c_ja;&c_d;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_a;&c_v;&c_t;&c_o;&c_n;&c_o;&c_m;&c_ib;&c_ja;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_o;&c_v;&c_l;&c_a;&c_d;&c_a;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="reg.super0i">
  <kap>super<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="reg.super0i.potence">
    <uzo tip="klr">(iu, io)</uzo>
    <dif>
      Havi pli da potenco ol:
      <ekz>
        la komando super<tld/>is la malamon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia timemo super<tld/>is
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>;
      </ekz>
      <ekz>
        lia koro batis forte, kaj ree super<tld/>ema sopiro ekposedis lin
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la da&ubreve;ro de kelke da momentoj &scirc;i ne povis eligi e&ccirc; unu vorton, poste,
        super<tld/>ante sin, &scirc;i ekparolis malla&ubreve;te
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iujn sentojn super<tld/>is en &scirc;i kortu&scirc;i&gcirc;o, dankemo
        <fnt>
          <bib>IK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="reg.super0i.nombre">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Esti pli grava, pli multnombra ol:
      <ekz>
        <klr>[&ccirc;io]</klr> en tiu tago nove kolori&gcirc;is de mortantaj homoj <klr>[&#8230;]</klr>,
        la rivereto havis ru&gcirc;an akvon <klr>[&#8230;]</klr>, la &ccirc;ie super<tld/>anta koloro
        malagrable brilis en la suno
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni komencis libere babili kaj &scirc;erci <klr>[&#8230;]</klr> post nelonge ilia aktiveco
        tute super<tld/>is tiujn de la aliaj
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la vojkruci&gcirc;oj staras anonctabuloj, nun anta&ubreve; somermezo super<tld/>as sur ili
        grandaj afi&scirc;oj pri danco kaj amuzi&gcirc;o
        <fnt>
          <bib>ViV</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="domin.0i">domini</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_v;&c_a;&c_zh;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_ja;&c_r;&c_sh;&c_e;&c_n;&c_s;&c_t;&c_v;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_m;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>p&rcaron;evl&aacute;dat</trd>,
    <trd>uplat&ncaron;ovat nadvl&aacute;du</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>vorherrschen</trd>,
    <trd>dominieren</trd>
  </trdgrp>
  <trd lng="en">overrule</trd>
  <trd lng="es">predominar</trd>
  <trdgrp lng="fr">
    <trd>dominer</trd>,
    <trd>maitriser</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>domin&aacute;l</trd>,
    <trd>ural</trd>
  </trdgrp>
  <trd lng="id"><baz>dominasi</baz>mendominasi</trd>
  <trdgrp lng="ja">
    <trd>&#22311;&#20498;&#12377;&#x308B;  <pr>&#12354;&#12387;&#12392;&#12358;&#12377;&#x308B;</pr></trd>,
    <trd>&#x512A;&#x8D8A;&#12377;&#x308B;  <pr>&#12422;&#12358;&#12360;&#12388;&#12377;&#x308B;</pr></trd>,
    <trd>&#x512A;&#20808;&#12377;&#x308B;  <pr>&#12422;&#12358;&#x305B;&#12435;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">overheersen</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_o;&c_b;&c_l;&c_a;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_g;&c_o;&c_s;&c_p;&c_o;&c_d;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_g;&c_l;&c_a;&c_v;&c_je;&c_n;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">ovl&aacute;da&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_a;&c_zh;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_d;&c_o;&c_m;&c_ib;&c_n;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

