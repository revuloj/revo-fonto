<drv mrk="reg.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc mrk="reg.0i.KOMUNE">
    <gra><vspec>tr</vspec></gra>
    <dif>
      Efektive <ref tip="super" cel="estr.0i">estri</ref>, <ref tip="super"
      cel="direkt.0i.ies_konduton">direkti</ref>,
      <ref tip="super" cel="obe.0igi">obeigi</ref>:
      <ekz>
        <tld/>i la <klr>[&#x2026;]</klr> glavon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iun tagon li faradis al &scirc;i sian proponon kaj volis fari &scirc;in <tld/>anta sinjorino
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>anta klaso parolis la francan
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu lernejestro klopodas siamaniere <tld/>i la situacion
        <fnt>
          <bib>Monato</bib>
          <aut>Gbeglo Koffi</aut>
          <vrk><url ref="&Monato;/2006/008960.php">Mur-muroj</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu celado de unu gento <tld/>i super aliaj gentoj, <klr>[&#x2026;]</klr> devos malaperi
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Homaranismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        lingvoj <klr>[&#x2026;]</klr> ne estas <tld/>ataj de biologiaj, sed de sociologiaj kaj
        lingvistikaj le&gcirc;oj
        <fnt>
          <bib>EfI</bib>
        </fnt>;
      </ekz>
      <ekz>
        mono mondon <tld/>as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        pli facile estas <tld/>i ol agi
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <subsnc mrk="reg.0i.POL">
      <dif>
        Direkti la aferojn de la <ref cel="sxtat.0o">&scirc;tato</ref>:
        <ekz>
          multaj faraonoj <tld/>is mallonge, kaj de kelkaj malaperis <klr>[&#x2026;]</klr> e&ccirc; la
          nomoj
          <fnt>
            <bib>Far1</bib>
            <lok>Anta&ubreve;parolo</lok>
          </fnt>;
        </ekz>
      <ekz>
        mi instruos al vi la malfacilan arton <tld/>i la &scirc;taton kaj observi la agojn de
        l' malamikoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
        <ekz>
          en tiu tempo la Fili&scirc;toj <tld/>is super <nom>Izrael</nom>
          <fnt>
            <bib>MT</bib>
            <lok>Ju&gcirc;istoj 14:4</lok>
          </fnt>;
        </ekz>
        <ekz>
          sur la insulo <tld/>as tre demokrata, parlamenta estraro
          <fnt>
            <bib>VaK</bib>
          </fnt>;
        </ekz>
        <ekz>
          <tld/>i oni povas super la lando; <ref cel="guvern.0i">guverni</ref>
          oni povas ian provincon (trovante sin tamen sub ordonoj pli altaj,
          ekzemple de re&gcirc;o) a&ubreve; ian personon
          <fnt>
            <bib>LR</bib>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
         <ref cel="regx.0i.POL">re&gcirc;i</ref>
      </refgrp>
      <trd lng="de">regieren</trd>
      <trd lng="hu">uralkodik</trd>
      <trd lng="nl">regeren</trd>
      <trdgrp lng="ru">
        <trd>&c_p;&c_r;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>,
        <trd>&c_v;&c_l;&c_a;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="reg.0i.EKON">
      <dif>
        Direkti kaj estri iun entreprenon:
        <ekz>
          forlasis sian metion pro diversaj ka&ubreve;zoj, sed precipe pro ribelo kontra&ubreve; la
          reguloj kaj postuloj de estre <tld/>ata laboro
          <fnt>
            <bib>KrM</bib>
          </fnt>;
        </ekz>
        <ekz>
          apud <tld/>anta &leftquot;Centra Komitato&rightquot; el 25 membroj, estis proponitaj i.a. unu
          15-membra &leftquot;Komitato de la Lingvo&rightquot;
          <fnt>
            <bib>EeP</bib>
          </fnt>;
        </ekz>
        <ekz>
          la varieteo estis <tld/>ata de komikulo nomata <nom>Gus Stratton</nom>
          <fnt>
            <bib>KrM</bib>
          </fnt>;
        </ekz>
        <ekz>
          brava mastrino tie <tld/>is <klr>[&#x2026;]</klr> &ccirc;io en la domo brilis kaj fulmis de
          pureco
          <fnt>
            <bib>Fab4</bib>
            <lok>Peco da perlovico</lok>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="dikt.0i">dikti</ref>,
        <ref cel="gvid.0i">gvidi</ref>,
        <ref cel="komand.0i">komandi</ref>,
        <ref cel="konduk.0i">konduki</ref>,
        <ref cel="mastr.0i">mastri</ref>,
        <ref cel="stir.0i">stiri</ref>
      </refgrp>
      <trdgrp lng="hu">
        <trd>ir&aacute;ny&iacute;t</trd>,
        <trd>vezet</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>besturen</trd>
      </trdgrp>
      <trd lng="ru">&c_u;&c_p;&c_r;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>
    </subsnc>
    <trdgrp lng="be">
      <trd>&c_k;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_k;&c_a;&c_n;&c_t;&c_r;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_d;&c_p;&c_a;&c_r;&c_a;&c_d;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
