<kap>
  <rad>tred</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="tred.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="tred.0i.JUV">
    <dif>
      <ref tip="super" cel="tra.0igi">Traigi</ref>, tiri de unu
      flanko &gcirc;is alia flanko de truo:
      <ekz>
        alia ludo <klr>...</klr> postulas &ccirc;iujn ludantojn <tld/>i
        ru&gcirc;an
        fadenon en 7 kudriltruojn en sama vico sur tabulo
        <fnt>
          <aut>Gao Xin</aut>,
          <vrk>
<url ref="http://esperanto.china.org.cn/kulturo/2007-06/12/ content_8377993.htm">
          Geamanta tago en &Ccirc;inio</url></vrk>,
          <lok>[2007]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_c;&c_ja;&c_g;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_c;&c_ja;&c_g;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_j;&c_m;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_n;&c_i;&c_sh;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>prostr&ccaron;it</trd>,
    <trd>protahovat</trd>
  </trdgrp>
  <trd lng="de">einf&auml;deln</trd>
  <trd lng="en">thread</trd>
  <trd lng="es">enhebrar</trd>
  <trd lng="fa">&a_n;&a_kh; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">enfiler</trd>
  <trd lng="hu">bef&udblac;z</trd>
  <trd lng="it">infilare<klr> (attraverso buchi)</klr></trd>
  <trd lng="ja">&#x901A;&#12377;  <pr>&#12392;&#x304A;&#12377;</pr></trd>
  <trd lng="nl">vademen<klr tip="ind"> (draad in naald bijv.)</klr></trd>
  <trd lng="pl">przewleka&cacute;</trd>
  <trd lng="sk">navliec&tcaron; <klr>(ni&tcaron;| &scaron;n&uacute;rku ap.)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&#x43E;&c_s;&c_i;&#x43B;&#x44F;&c_t;&c_i; (&#x43D;&c_i;&c_t;&#x43A;&c_u;</trd>,
    <trd>&c_sh;&#x43D;&c_u;&c_r;&#x43E;&#x43A; &c_ib; &c_t;.&#x43F;.)</trd>,
    <trd>&c_v;&c_s;&c_i;&#x43B;&#x44F;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
