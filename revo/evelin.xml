    <kap>
      <rad>Evelin</rad>/o
    </kap>

<drv mrk="evelin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">NOM</uzo>
  <snc>
    <dif>
      Virina <ref tip="lst" lst="voko:personaj_nomoj" cel="nom.persona0o">persona nomo</ref> angladevena:
      <ekz>
        mi dezirus vidi sinjorinon <tld/>on
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o &dash; &scirc;i sidis &ccirc;e la fenestro kaj spektis la vesperon invadanta la avenuon
        <fnt>
          <aut>James Joyce, trad. Russ Williams</aut>
          <vrk><url ref="http://esperanto-ondo.ru/Tl-ang11.htm"
              >Evelino</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Evelin</trd>
    <trd lng="en">Evelyn</trd>
    <trd lng="fr">Eveline</trd>
  </snc>
</drv>

  </art>

