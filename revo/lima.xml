<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>lima</rad>/o
</kap>
<drv mrk="lima.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="lima.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Lima</trd>), mara molusko kies konko havas
      &leftquot;orelojn&rightquot;.
    </dif>
  </snc>
  <trd lng="de">Feilenmuschel</trd>
</drv>
<drv mrk="lima.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="valv.du0uloj">duvalvuloj</ref>
      (<trd lng="la">Limidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Feilenmuscheln <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
