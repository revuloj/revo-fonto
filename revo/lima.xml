          <lok><url ref="&WCW;/File:Lima_scabra.jpg">&WCW;/File:Lima_scabra.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_m;&c_a; <klr tip="amb">(&c_m;&c_a;&c_l;&c_ju;&c_s;&c_k;)</klr></trd>
  <trd lng="de">Feilenmuschel</trd>
  <trd lng="en">Lima</trd>
  <trd lng="fr">lime<klr> (mollusque)</klr></trd>
  <trd lng="hu">reszel&odblac;kagyl&oacute;</trd>
  <trd lng="ja">&#x30DF;&#x30CE;&#x30AC;&#x30A4;</trd>
  <trd lng="nl">lima</trd>
  <trd lng="pl">lima</trd>
  <trd lng="uk">&#x43B;&c_ib;&#x43C;&c_a;</trd>
</drv>

