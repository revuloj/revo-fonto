<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tajvan</rad>/o
</kap>

<drv mrk="tajvan.0o">
  <kap>
    <tld lit="T"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="tajvan.0o.insulo">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Insulo en orienta <ref tip="malprt" cel="azi.A0o">Azio</ref>.
      Iam nomata Formozo.
      Teritorio de <ref tip="vid" cel="tajvan.0o.sxtato">Tajvano</ref>.
    </dif>
    <trd lng="bg">&c_T;&c_a;&c_j;&c_v;&c_a;&c_n;</trd>
    <trdgrp lng="ca">
      <trd>Taiwan</trd>,
      <trd>Formosa</trd>
    </trdgrp>
    <trd lng="de">Taiwan</trd>
    <trd lng="en">Taiwan</trd>
    <trdgrp lng="pl">
      <trd>Tajwan</trd>,
      <trd>Formoza<klr> (przest.)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="tajvan.0o.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      &Scirc;tato (konsiderata anka&ubreve; &Ccirc;ina respubliko) en la
      samnoma insulo kun &ccirc;efurbo Tajpeo.
      Respubliko Tajvano.
      ISO-3166 kodo: TW.
    </dif>
    <trdgrp lng="ca">
      <trd>Rep&uacute;blica de la Xina</trd>,
      <trd>Taiwan</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>Tajwan</trd>
    </trdgrp>
  </snc>
</drv>

</art>

