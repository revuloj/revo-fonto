<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kredenc.xml,v 1.12 2024/02/27 09:23:38 revo Exp $">
<kap>
  <rad>kredenc</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kredenc.0o">
  <kap><tld/>o</kap>
  <snc mrk="kredenc.0o.MEBL">
    <dif>
      <ref tip="lst" cel="mebl.0o.MEBL" lst="voko:mebloj">Meblo</ref>,
      malalta &scirc;ranko kun bretoj kaj pordoj,
      en kiu oni tenas <ref tip="vid" cel="servic.0o.KUI">servicojn</ref>:
      man&gcirc;ilojn, vazarojn, tablotukojn... kaj sur kies plato oni preparas man&gcirc;a&jcirc;ojn,
      iafoje anka&ubreve; kun supra, malfermita parto elmontranta
      belajn pecojn:
      <ekz>
        <tld/>o <klr>[&#8230;]</klr> frontas al la man&gcirc;ejo
        <fnt>
          <aut>Hajnalka Z.</aut>,
          <vrk><url ref="http://www.zsorihajnalka.hu/konyhak3_1_esp.htm">
          Kuirejoj, man&gcirc;ejoj, 1</url></vrk>,
          <lok>2009-2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        malmultekosta &ccirc;ina ludilo <klr>[kiun]</klr> &scirc;i zorgeme starigis <klr>[&#8230;]</klr>
        sur <tld/>on a&ubreve; tablon, de kie &gcirc;i ludeme kaj intime, kun ega &scirc;argo da
        optimismo, gaje rigardis al &scirc;i per la rondaj smeraldaj okuloj
        <fnt>
          <aut>Espopore</aut>
          <vrk><url ref="&Monato;/2019/pdf/monato201901.pdf#page=25">Smeraldaj okuloj</url></vrk>
          <bib>Monato</bib>
          <lok>2019/01, p. 25</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bufed.0o.MEBL">bufedo<sncref/></ref>,
      <ref cel="stabl.0o.metitablo">stablo</ref>
    </refgrp>
  </snc>
  <trd lng="ar">&a_m;&a_q;&a_S;&a_f; <klr>(&a_A_hamza_sure;&a_th;&a_A;&a_th;)</klr></trd>
  <trd lng="be">&c_b;&c_u;&c_f;&c_e;&c_t; <klr tip="amb">(&c_sh;&c_a;&c_f;&c_a;)</klr></trd>
  <trdgrp lng="de">
    <trd>Anrichte</trd>,
    <trd>Serviertisch</trd>
    <trd>Kredenz</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>sideboard</trd>,
    <trd>buffet</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cr&eacute;dence</trd>,
    <trd>buffet<klr> (meuble)</klr></trd>,
    <trd>dressoir</trd>,
    <trd>vaisselier</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>aanrecht</trd>,
    <trd>dressoir</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kredens</trd>,
    <trd>kredencja</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>creden&ccedil;a</trd>,
    <trd>cred&ecirc;ncia</trd>
  </trdgrp>
  <trd lng="ro">bufet</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_r;&c_je;&c_d;&c_je;&#x43D;&c_s;</trd>,
    <trd>&c_b;&c_u;&c_f;&c_je;&c_t; &c_d;&#x43B;&#x44F; &#x43F;&#x43E;&c_s;&c_u;&c_d;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20633;&#39184;&#38291; <pr>b&#xE8;ic&amacron;nji&amacron;n</pr></trd>,
    <trd>&#22791;&#39184;&#x95F4; <pr>b&#xE8;ic&amacron;nji&amacron;n</pr></trd>,
    <trd>&#39184;&#20855;&#x67DC; <pr>c&amacron;nj&#xF9;gu&#xEC;</pr></trd>,
    <trd>&#39184;&#20855;&#x6AC3; <pr>c&amacron;nj&#xF9;gu&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>

