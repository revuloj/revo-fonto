<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kredenc</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kredenc.0o">
  <kap><tld/>o</kap>
  <snc mrk="kredenc.0o.MEBL">
    <dif>
      <ref tip="lst" cel="mebl.0o.MEBL" lst="voko:mebloj">Meblo</ref>,
      malalta &scirc;ranko kun bretoj kaj pordoj,
      en kiu oni tenas
      <ref tip="vid" cel="servic.0o.KUI">servicojn</ref>:
      man&gcirc;ilojn, vazarojn, tablotukojn...,
      iafoje anka&ubreve; kun supra, malfermita parto elmontranta
      belajn pecojn:
      <ekz>
        <tld/>o <klr>...</klr> frontas al la man&gcirc;ejo
        <fnt>
          <aut>Hajnalka Z.</aut>,
          <vrk><url
          ref="http://www.zsorihajnalka.hu/konyhak3_1_esp.htm">
          Kuirejoj, man&gcirc;ejoj, 1</url></vrk>,
          <lok>2009-2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>cr&eacute;dence</trd>,
    <trd>buffet<klr> (meuble)</klr></trd>,
    <trd>dressoir</trd>,
    <trd>vaisselier</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kredens</trd>,
    <trd>kredencja</trd>
  </trdgrp> 
  <trd lng="ro">bufet</trd> 
</drv>
</art>

