<rad>arom</rad>/o
</kap>
<drv mrk="arom.0o">
  <kap><tld/>o</kap>
  <snc mrk="arom.0o.SEN">
    <dif>
      Delikata <ref tip="super" cel="odor.bon0o.SEN">bonodoro</ref>,
      precipe efikanta al la gustorgano:
      <ekz>
        <tld/>o de teo, de kafo, de vanilo;
      </ekz>
      <ekz>
        <tld/>o de spicoj kaj floroj levi&gcirc;is al ili
        <fnt><bib>Fab1</bib> <lok>&Gcirc;ardeno de la paradizo</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;iu floro havis balzaman <tld/>on
        <fnt><bib>Fab2</bib> <lok>Dio de dormo</lok></fnt>;
      </ekz>
      <ekz>
        &gcirc;usta printempa tago por poeto,
        <klr>[&#8230;]</klr> en la verda&jcirc;o estas tia <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de &jcirc;us plugita kampo
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj la&ubreve;rarboj
        <fnt><bib>Fab1</bib> <lok>Galo&scirc;oj de feli&ccirc;o</lok></fnt>;
      </ekz>
      <ekz>
        mi dormas sub <klr>[&#8230;]</klr> <tld/>aj malsuprenpendantaj bran&ccirc;oj
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>on de la realeco
        ili <klr>(tiuj rakontoj)</klr> ne povas transdoni
        <fnt><bib>Fab1</bib>, <lok>Ligo de amikeco</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ni amba&ubreve; enspiradis per plenaj pulmoj la <tld/>on de romaneco, kiu trovi&gcirc;is en
        nia tuta aventuro
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="parfum.0o.bonodoro">parfumo<sncref/></ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#x6D53;&#39321; <pr>n&#xF3;ngxi&amacron;ng</pr></trd>,
    <trd>&#x6FC3;&#39321; <pr>n&#xF3;ngxi&amacron;ng</pr></trd>,
    <trd>&#x6ECB;&#21619; <pr>z&#x12B;w&#xE8;i</pr></trd>,
    <trd>&#x6C14;&#21619; <pr>q&#xEC;w&#xE8;i</pr></trd>,
    <trd>&#x6C23;&#21619; <pr>q&#xEC;w&#xE8;i</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_o;&c_d;&c_a;&c_r;</trd>,
    <trd>&c_p;&c_a;&c_h;</trd>,
    <trd>&c_p;&c_a;&c_h;&c_u;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_u;&c_h;&c_m;&c_ja;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">aroma</trd>
  <trdgrp lng="cs">
    <trd>ar&oacute;ma</trd>,
    <trd>v&uring;n&ecaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Aroma</trd>,
    <trd>Duft</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>aroma</trd>,
    <trd>smell</trd>,
    <trd>fragrance</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>aroma</trd>,
    <trd>olor</trd>
  </trdgrp>
  <trd lng="fr">ar&ocirc;me</trd>
  <trdgrp lng="he">
    <trd>&alef;&#x5B2;&resh;&vav;&#x5B9;&mem;&#x5B8;&he;</trd>,
    <trd>&nun;&yod;&het;&vav;&het;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>aroma</trd>,
    <trd>illat</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>aroma</trd>,
    <trd>profumo</trd>,
    <trd>fragranza</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x82B3;&#39321;  <pr>&#x307B;&#12358;&#12371;&#12358;</pr></trd>,
    <trd>&#39321;&#x6C17;  <pr>&#12371;&#12358;&#x304D;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>aroma</trd>,
    <trd>geur</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>aromat</trd>,
    <trd>zapach</trd>,
    <trd>wo&nacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_o;&c_m;&c_a;&c_t;</trd>,
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_u;&c_h;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>v&ocirc;&ncaron;a</trd>,
    <trd>v&ocirc;&ncaron;a</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>arom</trd>,
    <trd>doft</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&c_r;&c_o;&c_m;&c_a;&c_t;</trd>,
    <trd>&c_p;&c_a;&c_h;&c_o;&c_shch;&c_ib;</trd>
  </trdgrp>
</drv>

<drv mrk="arom.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="arom.0i.SEN">
    <dif>
      Havi <tld/>on, <ref tip="vid" cel="odor.bon0i">bonodori</ref>:
      <ekz>
        balda&ubreve; la kafo <tld/>is en la &ccirc;ambro
        <fnt>
          <aut>H. Laxness, trad. B. Ragnarsson</aut>
          <vrk>Sendependaj homoj</vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        forte <tld/>is poploj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la kampo <tld/>is fre&scirc;e fal&ccirc;ita fojno
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        loge <tld/>as parfum' de l' aniz'
        <fnt>
          <bib>LSB</bib>
          <lok>Moderna An&gcirc;eluso</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x53D1;&#x51FA;&#39321;&#21619; <pr>f&amacron;ch&#x16B;xi&amacron;ngw&#xE8;i</pr></trd>,
    <trd>&#x767C;&#x51FA;&#39321;&#21619; <pr>f&amacron;ch&#x16B;xi&amacron;ngw&#xE8;i</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_p;&c_a;&c_h;&c_n;&c_u;&c_c;&c_mol; <klr>(&c_d;&c_o;&c_b;&c_r;&c_a;)</klr></trd>
  <trd lng="ca">desprendre aroma</trd>
  <trd lng="cs">von&ecaron;t</trd>
  <trdgrp lng="de">
    <trd>Aroma <ind>verstr&ouml;men</ind></trd>,
    <trd>duften</trd>
  </trdgrp>
  <trd lng="en">smell<klr> (like)</klr></trd>
  <trd lng="es">oler</trd>
  <trdgrp lng="fr">
    <trd>r&eacute;pandre un <ind>ar&ocirc;me</ind></trd>,
    <trd>embaumer<klr tip="ind"> (intr.)</klr></trd>
  </trdgrp>
  <trd lng="hu">illatozik</trd>
  <trd lng="it">profumare<klr> (intr)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x304B;&#x304A;&#x308B;</trd>,
    <trd>&#x82B3;&#39321;&#12434;&#x653E;&#12388;  <pr>&#x307B;&#12358;&#12371;&#12358;&#12434;&#x306F;&#x306A;&#12388;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>geuren</trd>,
    <trd>ruiken<klr tip="ind"> (geuren)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pachn&aogonek;&cacute; <ind>aromatycznie</ind></trd>,
    <trd>mie&cacute; <ind>aromat</ind></trd>,
    <trd>wydziela&cacute; wo&nacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_l;&c_a;&c_g;&c_o;&c_u;&c_h;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">vo&ncaron;a&tcaron;</trd>
</drv>

<drv mrk="arom.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="arom.0ajxo.KOMUNE">
    <dif>
      Substanco havanta <tld/>on:
      <ekz>
        kameloj portas <tld/>a&jcirc;ojn kaj balzamon kaj mirhon
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 37:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        prenu al vi plej bonajn <tld/>a&jcirc;ojn: da bonodora mirho <klr>[&#8230;]</klr>, da
        <tld/>a cinamo <klr>[&#8230;]</klr> kaj da <tld/>a kano
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 30:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni bezonas malgrandan oleolampon <klr>[por]</klr> ekbruligi <tld/>a&jcirc;on
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la incensiloj oni ekbruligis <tld/>a&jcirc;ojn
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#8230;]</klr> ver&scirc;is tro da ostra sa&ubreve;co, da <tld/>a&jcirc;o, a&ubreve; ion similan en
        &scirc;ian man&gcirc;a&jcirc;on
        <fnt>
          <bib>ChC</bib>
          <lok>14</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="herb.arom0o">aromherbo</ref>,
      <ref cel="incens.0o">incenso</ref>,
      <ref cel="parfum.0ajxo">parfuma&jcirc;o</ref>,
      <ref cel="spic.0ajxo">spica&jcirc;o</ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#x8ABF;&#21619;&#21137; <pr>ti&#xE1;ow&#xE8;ij&#xEC;</pr></trd>,
    <trd>&#x8C03;&#21619;&#21058; <pr>ti&#xE1;ow&#xE8;ij&#xEC;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_r;&c_a;&c_m;&c_a;&c_t;&c_y;&c_z;&c_a;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_p;&c_a;&c_h;&c_u;&c_ch;&c_a;&c_je; <ind>&c_r;&c_e;&c_ch;&c_y;&c_v;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="ca">subst&agrave;ncia arom&agrave;tica</trd>
  <trd lng="de">Aroma</trd>
  <trd lng="en">aromatic</trd>
  <trdgrp lng="fr">
    <trd>ar&ocirc;me</trd>,
    <trd>aromate</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>aroma(anyag)</trd>,
    <trd>eszencia</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>aroma<klr> (sostanza)</klr></trd>,
    <trd>essenza<klr> (sostanza aromatica)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#39321;&#26009;  <pr>&#12371;&#12358;&#x308A;&#12423;&#12358;</pr></trd>,
    <trd>&#x82B3;&#39321;&#21092;  <pr>&#x307B;&#12358;&#12371;&#12358;&#12374;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>aromaat</trd>,
    <trd>geurstof</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pachnid&lstroke;o</trd>,
    <trd>substancja <ind>aromatyczna</ind></trd>,
    <trd>przyprawa <ind>aromatyczna</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_r;&c_o;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; &c_v;&c_je;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="uk">&c_z;&c_a;&c_p;&c_a;&c_sh;&c_n;&c_a; &c_r;&c_je;&c_ch;&c_o;&c_v;&c_i;&c_n;&c_a;</trd>
</drv>

<drv mrk="arom.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="arom.0igi.KOMUNE">
    <dif>
      <ref cel="parfum.0i">Parfumi</ref> ion per <tld/>a&jcirc;o:
      <ekz>
        bukedoj da floroj <tld/>igis la
        aeron.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="spic.0i">spici</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; <ind>&c_p;&c_a;&c_h;</ind></trd>,
    <trd>&c_a;&c_r;&c_a;&c_m;&c_a;&c_t;&c_y;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>aromar</trd>,
    <trd>aromatitzar</trd>
  </trdgrp>
  <trd lng="cs">navon&ecaron;t</trd>
  <trd lng="de">aromatisieren</trd>
  <trd lng="en">aromatize</trd>
  <trd lng="es">aromatizar</trd>
  <trd lng="fr">aromatiser</trd>
  <trd lng="hu">illatos&iacute;t</trd>
  <trdgrp lng="it">
    <trd>profumare<klr> (dare aroma)</klr></trd>,
    <trd>aromatizzare</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#39321;&#26009;&#12434;&#20837;&#x308C;&#x308B;  <pr>&#12371;&#12358;&#x308A;&#12423;&#12358;&#12434;&#12356;&#x308C;&#x308B;</pr></trd>,
    <trd>&#39321;&#x308A;&#12434;&#x6DFB;&#12360;&#x308B;  <pr>&#x304B;&#x304A;&#x308A;&#12434;&#x305D;&#12360;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">aromatiseren</trd>
  <trdgrp lng="pl">
    <trd>aromatyzowa&cacute;</trd>,
    <trd>nadawa&cacute; <ind>aromat</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; <ind>&c_d;&c_u;&c_sh;&c_i;&c_s;&c_t;&c_y;&c_m;</ind></trd>,
    <trd>&c_a;&c_r;&c_o;&c_m;&c_a;&c_t;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>aromatizova&tcaron;</trd>,
    <trd>navo&ncaron;a&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_b;&c_i;&c_t;&c_i; &c_z;&c_a;&c_p;&c_a;&c_sh;&c_n;&c_i;&c_m;</trd>,
    <trd>&c_a;&c_r;&c_o;&c_m;&c_a;&c_t;&c_n;&c_i;&c_m;</trd>,
    <trd>&c_a;&c_r;&c_o;&c_m;&c_a;&c_t;&c_i;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
 </drv>
