<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>arom</rad>/o
</kap>
<drv mrk="arom.0o">
   <kap><tld/>o</kap>
   <snc>
     <dif>
       Delikata, agrabla odoro, parfumo, precipe  efikanta sur la gustorgano:
       <ekz>
         <tld/>o de teo, de kafo, de vanilo;
       </ekz>
       <ekz>
         <tld/>o de spicoj kaj floroj levi&gcirc;is al ili
         <fnt>(<aut>Zamenhof</aut>: <vrk>Fabeloj de Andersen,
         &Gcirc;ardeno de la paradizo</vrk>)</fnt>;
       </ekz>
       <ekz>
         &ccirc;iu floro havis balzaman <tld/>on
         <fnt>(<aut>Zamenhof</aut>: <vrk>Fabeloj de Andersen,
         Dio de dormo</vrk>)</fnt>;
       </ekz>
       <ekz>
         <tld/>a likvoro<fnt>Z</fnt>, </ekz> <ekz>aero<fnt>Z</fnt>;
       </ekz>
       <ekz>
         <tld/>aj la&ubreve;rarboj
         <fnt>(<aut>Zamenhof</aut>: <vrk>Fabeloj de Andersen,
         Galo&scirc;oj de feli&ccirc;o</vrk>)</fnt>;
       </ekz>
       <ekz>
         <tld/>aj bran&ccirc;oj<fnt>Z</fnt>;
       </ekz>
       <ekz>
         <klr>(f)</klr> la <tld/>on de la realeco
         ili <klr>(tiuj rakontoj)</klr> ne povas transdoni
         <fnt>(<aut>Zamenhof</aut>: <vrk>Fabeloj de Andersen,
         Ligo de amikeco</vrk>)</fnt>;
       </ekz>
       <ekz>
         <tld/>o de romantikeco.
       </ekz>
     </dif>
   </snc>
   <trd lng="de">Aroma</trd>
   <trd lng="fr">ar&ocirc;me</trd>
   <trdgrp lng="ru">
     <trd>&#1072;&#1088;&#1086;&#1084;&#1072;&#1090;</trd>,
     <trd>&#1073;&#1083;&#1072;&#1075;&#1086;&#1091;&#1093;&#1072;&#1085;&#1080;&#1077;</trd>
   </trdgrp>
</drv>
<drv mrk="arom.0ajxo">
   <kap><tld/>a&jcirc;o</kap>
   <snc>
     <dif>
       Substanco havanta <tld/>on.
     </dif>
   </snc>
   <trd lng="de">Aroma</trd>
   <trdgrp lng="fr">
     <trd>ar&ocirc;me</trd>,
     <trd>aromate</trd></trdgrp>
</drv>
<drv mrk="arom.0i">
   <kap><tld/>i</kap>
   <gra><vspec>ntr</vspec></gra>
   <snc>
     <dif>
       Havi <tld/>on.
     </dif>
   </snc>
   <trdgrp lng="fr">
     <trd>r&eacute;pandre un <ind>ar&ocirc;me</ind></trd>,
     <trd>enbaumer (intr.)</trd></trdgrp>
   <trd 
lng="ru">&#1073;&#1083;&#1072;&#1075;&#1086;&#1091;&#1093;&#1072;&#1090;&#1100;</trd>
</drv>
<drv mrk="arom.0igi">
   <kap><tld/>igi</kap>
   <snc>
     <dif>
       Parfumi ion per <tld/>a&jcirc;o:
       <ekz>
         bukedoj da floroj <tld/>igis la
         aeron.
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="odor.0o">odoro</ref>,
       <ref cel="spic.0o">spico</ref>.
     </refgrp>
   </snc>
   <trd lng="de">aromatisieren</trd>
   <trd lng="fr">aromatiser</trd>
   <trdgrp lng="ru">
     <trd>&#1076;&#1077;&#1083;&#1072;&#1090;&#1100; 
&#1076;&#1091;&#1096;&#1080;&#1089;&#1090;&#1099;&#1084;</trd>,
     <trd>&#1072;&#1088;&#1086;&#1084;&#1072;&#1090;&#1080;&#1079;&#1080;&#1088;&#1086;&#1074;&#1072;&#1090;&#1100;</trd>
   </trdgrp>
</drv>
<drv mrk="arom.0herbo">
   <kap><tld/>herbo</kap>
   <uzo tip="fak">KUI</uzo>
   <snc>
     <dif>
       Herbo uzata por <tld/>igi pladojn.
     </dif>
   </snc>
   <trdgrp lng="fr">
     <trd>aromate</trd>,
     <trd>herbe <ind>aromatique</ind></trd></trdgrp>
</drv>
</art>
