        Grin&ccaron;evi&ccaron;ius</aut>: <vrk>Ruze eniri, akuze eliri</vrk>,
        <bib>Monato</bib><lok>jaro 2000a, numero 11a, p. 6a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_a;&c_zh;&c_y;&c_r;</trd>
  <trd lng="bg">&c_p;&c_malmol;&c_t;&c_n;&c_i;&c_k;</trd>
  <trd lng="bo">&#xF60;&#xF42;&#xFB2;&#xF74;&#xF63;&#xF0B;&#xF54;&#xF0B;</trd>
  <trdgrp lng="ca">
    <trd>passatger</trd>,
    <trd>viatger</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Passagier</trd>,
    <trd>Reisender</trd>,
    <trd>Fahrgast</trd>,
    <trd>Fluggast</trd>
  </trdgrp>
  <trd lng="en">passenger</trd>
  <trdgrp lng="es">
    <trd>pasajero</trd>,
    <trd>viajero</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>passager</trd>,
    <trd>voyageur</trd>
  </trdgrp>
  <trd lng="hu">utas</trd>
  <trd lng="id"><baz>tumpang</baz>penumpang</trd>
  <trdgrp lng="ja">
    <trd>&#x4E57;&#x5BA2;  <pr>&#x3058;&#x3087;&#x3046;&#x304D;&#x3083;&#x304F;</pr></trd>,
    <trd>&#x65C5;&#x5BA2;  <pr>&#x308A;&#x3087;&#x304B;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">passagier</trd>
  <trdgrp lng="pl">
  <trd>pasa&zdot;er</trd>,
  <trd>podr&oacute;&zdot;ny</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_s;&c_a;&c_zh;&c_i;&c_r;</trd>
  <trd lng="uk">&#x43F;&#x430;&#x441;&#x430;&#x436;&#x438;&#x440;</trd>
  <trd lng="zh">&#x4E58;&#x5BA2; <pr>ch&#xE9;ngk&#xE8;</pr></trd>
