<kap>
  <ofc>1</ofc>
  <rad>teolog</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="teolog.0o">
  <kap><tld/>o</kap>
  <snc mrk="teolog.0o.REL">
    <dif>
      <ref tip="dif" cel="teolog1.0isto">Teologiisto</ref>:
      <ekz>
        kiel mu&scirc;oj &ccirc;irka&ubreve; sukero a&ubreve; <tld/>o &ccirc;irka&ubreve; paro&hcirc;o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;armaj duonnudaj infanoj gardis amason da karbonigraj 
        porkoj sub grupo da aromaj la&ubreve;rarboj &ccirc;e la vojo
        <klr>[&#8230;]</klr> &leftquot;ho belega Italujo!&rightquot;, sed tion ne diris
        la <tld/>o nek e&ccirc; unu el liaj voja&gcirc;kamaradoj en la kale&scirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Anka&#365; eblas diri <ke>difakulo</ke>, <ke>disciencisto</ke>, <ke>diesploranto</ke>.
  </rim>

  <trd lng="be">&c_t;&c_e;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="bg">&c_t;&c_je;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="ca">te&ograve;leg</trd>
  <trdgrp lng="cs">
    <trd>bohoslovec</trd>,
    <trd>teolog</trd>
  </trdgrp>
  <trd lng="de">Theologe</trd>
  <trd lng="en">theologist</trd>
  <trd lng="fr">th&eacute;ologien</trd>
  <trdgrp lng="hu">
    <trd>teol&oacute;gus</trd>,
    <trd>hittud&oacute;s</trd>
  </trdgrp>
  <trd lng="ja">&#31070;&#23398;&#32773;  <pr>&#12375;&#12435;&#12364;&#12367;&#12375;&#12419;</pr></trd>
  <trd lng="nl">teoloog</trd>
  <trd lng="pl">teolog</trd>
  <trd lng="pt">te&oacute;logo</trd>
  <trd lng="ru">&c_t;&c_je;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="sk">bohoslovec</trd>
  <trdgrp lng="uk">
    <trd>&#1090;&#1077;&#1086;&#1083;&#1086;&#1075;</trd>,
    <trd>&#1073;&#1086;&#1075;&#1086;&#1089;&#1083;&#1086;&#1074;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#31070;&#23398;&#23478; <pr>sh&#233;nxu&#233;ji&#257;</pr></trd>,
    <trd>&#31070;&#23416;&#23478; <pr>sh&#233;nxu&#233;ji&#257;</pr></trd>,
    <trd>&#31070;&#23398;&#30740;&#31350;&#32773; <pr>sh&#233;nxu&#233;y&#225;nji&#249;zh&#283;</pr></trd>,
    <trd>&#31070;&#23416;&#30740;&#31350;&#32773; <pr>sh&#233;nxu&#233;y&#225;nji&#249;zh&#283;</pr></trd>,
    <trd>&#31070;&#23398;&#32773; <pr>sh&#233;nxu&#233;zh&#283;</pr></trd>,
