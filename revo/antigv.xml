<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: antigv.xml,v 1.15 2023/10/28 08:33:07 revo Exp $">
<kap>
  <rad>Antigv</rad>/o
</kap>

<drv mrk="antigv.0o">
  <kap>
    <tld/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="antigv.0o.insulo">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" cel="insul.0o" lst="voko:insuloj">Insulo</ref>
      de Centra Ameriko en Kariba Maro, iama kolonio de Britujo,
      geografie apartenanta al Deventaj Insuloj:
      <ekz>
        <nom>Williams</nom> naski&gcirc;is <nom>Egbert Austin Williams</nom> sur la insulo de
        <tld/>o, tiam parto de la Britiaj Karibaj Antiloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=36139">Bert
          Williams</url></lok>
        </fnt>.
      </ekz>
    </dif>
      <trdgrp lng="zh">
    <trd>&#x5B89;&#x5730;&#x5361;&#x5C9B; <pr>&#x101;nd&#xEC;k&#x1CE;d&#x1CE;o</pr></trd>,
    <trd>&#x5B89;&#x5730;&#x5361;&#x5CF6; <pr>&#x101;nd&#xEC;k&#x1CE;d&#x1CE;o</pr></trd>
  </trdgrp>
</snc>
    <trd lng="be">&c_A;&c_n;&c_t;&c_y;&c_g;&c_u;&c_a;</trd>
    <trd lng="bg">&c_A;&c_n;&c_t;&c_i;&c_g;&c_u;&c_a;</trd>
    <trd lng="de">Antigua</trd>
    <trd lng="es">Antigua</trd>
  <trd lng="fr">Antigua</trd>
    <trd lng="id">Antigua</trd>
    <trdgrp lng="pl">
      <trd>Antigua</trd>
    </trdgrp>
</drv>

<drv mrk="antigv.0o_Barbudo">
  <kap>
    <tld/>o-Barbudo,
    <var>
      <kap>
        <tld/>o kaj Barbudo
        <fnt>
          <vrk>Oficiala Informo de AdE</vrk>,
          <lok>numero 11</lok>
        </fnt>
      </kap>
    </var>
  </kap>
  <mlg kod="ISO-3166">AG</mlg>
  <snc mrk="antigv.0o.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Insula
      <ref tip="lst" cel="stat.0o" lst="voko:&scirc;tatoj">&scirc;tato</ref>
      de Centra Ameriko en Kariba Maro kun
      &ccirc;efurbo Sankta Johano:
      <ekz>
        <tld/>o-Barbudo apartenas al la AKP-&scirc;tatoj, internacia
        organiza&jcirc;o de
        momente 77 nacioj el la regionoj Afriko, Karibio kaj la Pacifiko
        <fnt>
          <bib>Viki</bib>,
          <vrk><url ref="&Viki;/Antigvo-Barbudo">
          Antigvo-Barbudo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    </snc>
    <trd lng="be">&c_A;&c_n;&c_t;&c_y;&c_g;&c_u;&c_a; &c_ib; &c_B;&c_a;&c_r;&c_b;&c_u;&c_d;&c_a;</trd>
    <trd lng="bg">&c_A;&c_n;&c_t;&c_i;&c_g;&c_u;&c_a; &c_i;
    &c_B;&c_a;&c_r;&c_b;&c_u;&c_d;&c_a;</trd>
    <trd lng="de">Antigua und Barbuda</trd>
    <trd lng="es">Antigua y Barbuda</trd>
  <trd lng="fr">Antigua-et-Barbuda</trd>
    <trd lng="id">Antigua dan Barbuda</trd>
    <trd lng="ja">&#x30A2;&#x30F3;&#x30C6;&#x30A3;&#x30B0;&#x30A2;&#x30FB;&#x30D0;&#x30FC;&#x30D6;&#x30FC;&#x30C0;  <pr>&#x3042;&#x3093;&#x3066;&#x3043;&#x3050;&#x3042;&#x30FB;&#x3070;&#x30FC;&#x3076;&#x30FC;&#x3060;</pr></trd>
  <trdgrp lng="pl">
      <trd>Antigua i Barbuda</trd>
    </trdgrp>
    <trd lng="pt">Antiga e Barbuda</trd>
  <trd lng="uk">&c_A;&c_n;&c_t;&c_i;&c_gu;&c_u;&c_a; &c_t;&c_a; &c_B;&c_a;&c_r;&c_b;&c_u;&c_d;&c_a;</trd>
</drv>

</art>

