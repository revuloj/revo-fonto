<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>antigv</rad>/o
</kap>

<drv mrk="antigv.0o">
  <kap>
    <tld lit="A"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="antigv.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Insulo en la Centra Ameriko en Kariba Maro, en insularo Deventaj
      Insuloj,
      apartenanta al <ref tip="vid" cel="antigv.0o.sxtato.GEOG">Antigvo
      kaj Barbudo</ref>.
    </dif>
    <trdgrp lng="pl">
      <trd>Antigua</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="antigv.0o.sxtato">
  <kap>
    <tld lit="A"/>o kaj Barbudo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="antigv.0o.sxtato.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Insula &scirc;tato en la Centra Ameriko en Kariba Maro kun 
      &ccirc;efurbo Saint John's.
      ISO-3166 kodo: AG.
    </dif>
    <trdgrp lng="pl">
      <trd>Antigua i Barbuda</trd>
    </trdgrp>
  </snc>
</drv>

</art>

<!--
$Log$
-->
</vortaro>