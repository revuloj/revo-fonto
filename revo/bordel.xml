<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>bordel</rad>/o
</kap>
<drv mrk="bordel.0o">
<kap><tld/>o</kap>
<snc>
<dif>
Domo, aran&gcirc;ita por la komerco per 
<ref cel="prosti.0i">prostituado</ref>.
</dif>
<refgrp tip="sin">
  <ref cel="prosti.0ejo">prostituejo</ref>,
  <ref cel="cxast.mal0ejo">mal&ccirc;astejo</ref>.
</refgrp>
</snc>
<trdgrp lng="fr">
  <trd>bordel <klr tip="ind">(maison close)</klr></trd>,
  <trd>maison close</trd></trdgrp> 
<trdgrp lng="ru">
  <trd>&c_d;&c_o;&c_m; &c_t;&c_je;&c_r;&c_p;&c_i;&c_m;&c_o;&c_s;&c_t;&c_i;</trd>,
  <trd>&c_p;&c_u;&c_b;&c_l;&c_i;&c_ch;&c_n;&c_y;&c_j; &c_d;&c_o;&c_m;</trd>,
  <trd>&c_b;&c_o;&c_r;&c_d;&c_je;&c_l;&c_mol;</trd>
</trdgrp>
<trd lng="sv">bordell</trd>
</drv>
</art>
