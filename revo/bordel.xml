<kap>
  <ofc>1</ofc>
  <rad>bordel</rad>/o
</kap>
<drv mrk="bordel.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Domo, aran&gcirc;ita por la komerco per
      <ref cel="prosti.0i">prostituado</ref>:
      <ekz>
        la Kontrakto de Novjorko, subskribita en 1955, [...] malpermesas
        starigon de <tld/>oj<fnt><aut>Juh&aacute;sz K&aacute;roly</aut>:
        <vrk>Buda
        buduaro apud-budapesta</vrk>, <bib>Monato</bib><lok>jaro 2001a,
        numero 3a, p. 13a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="cxast.mal0ejo">mal&ccirc;astejo</ref>,
      <ref cel="prosti.0ejo">prostituejo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_r;&c_d;&c_e;&c_l;&c_mol;</trd>
  <trdgrp lng="bg">
    <trd>&c_p;&c_u;&c_b;&c_l;&c_i;&c_ch;&c_je;&c_n; &c_d;&c_o;&c_m;</trd>,
    <trd>&c_b;&c_o;&c_r;&c_d;&c_je;&c_j;</trd>
  </trdgrp>
  <trd lng="ca">bordell</trd>
  <trdgrp lng="cs">
    <trd>bordel</trd>,
    <trd>lupan&#xE1;r <klr>(zast.)</klr></trd>,
    <trd>lupan&#xE1;rium</trd>,
    <trd>nev&#x11B;stinec</trd>
  </trdgrp>
  <trd lng="de">Puff</trd>
  <trdgrp lng="en">
    <trd>bordello</trd>,
    <trd>brothel</trd>
  </trdgrp>
  <trd lng="es">burdel</trd>
  <trdgrp lng="fa">
    <trd>&a_r;&a_w;&a_s;&f_p;&f_y;&zwnj;&a_kh;&a_A;&a_n;&a_h;</trd>,
    <trd>&a_f;&a_A;&a_H;&a_sh;&a_h;&zwnj;&a_kh;&a_A;&a_n;&a_h;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bordel<klr tip="ind"> (maison close)</klr></trd>,
    <trd>maison close</trd>
  </trdgrp>
  <trd lng="he">&#x5D1;&#x5BC;&#x5B5;&#x5D9;&#x5EA; &#x5D1;&#x5D5;&#x5E9;&#x5EA;</trd>
  <trdgrp lng="it">
    <trd>bordello<klr> (casa chiusa)</klr></trd>,
    <trd>casa chiusa</trd>,
    <trd>casino<klr> (casa chiusa)</klr></trd>,
    <trd>casa di tolleranza</trd>
  </trdgrp>
  <trd lng="ja">&#x58F2;&#x6625;&#x5BBF;  <pr>&#x3070;&#x3044;&#x3057;&#x3085;&#x3093;&#x3084;&#x3069;</pr></trd>
  <trd lng="nl">bordeel</trd>
  <trdgrp lng="pl">
    <trd>burdel</trd>,
    <trd>dom publiczny</trd>,
    <trd>dom schadzek</trd>,
    <trd>dom rozkoszy</trd>,
    <trd>dom przyjemno&sacute;ci</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_m;
    &c_t;&c_je;&c_r;&c_p;&c_i;&c_m;&c_o;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_u;&c_b;&c_l;&c_i;&c_ch;&c_n;&c_y;&c_j;
    &c_d;&c_o;&c_m;</trd>,
    <trd>&c_b;&c_o;&c_r;&c_d;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bordel</trd>,
    <trd>dom prostit&#xFA;tok</trd>
  </trdgrp>
  <trd lng="sv">bordell</trd>
  <trdgrp lng="uk">
    <trd>&#x440;&#x43E;&#x437;&#x43F;&#x443;&#x441;&#x442;&#x438;</trd>,
    <trd>&#x431;&#x43E;&#x440;&#x434;&#x435;&#x43B;&#x44C;</trd>
  </trdgrp>
</drv>
