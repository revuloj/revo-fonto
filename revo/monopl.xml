<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>monoplan</rad>/o
</kap>
<drv mrk="monopl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AVI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="aeropl.0o">Aeroplano</ref> havanta unu paron
      da flugiloj:
      <ekz>
        la unua DC-3 el 1936 estis dumotora <tld/>o kun sube staranta flugilo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=115898"
              >Douglas DC-3</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="biplan.0o">biplano</ref>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_a;&c_p;&c_l;&c_ja;&c_n;</trd>
  <trd lng="bg">&c_m;&c_o;&c_n;&c_o;&c_p;&c_l;&c_a;&c_n;</trd>
  <trd lng="ca">monopl&agrave;</trd>
  <trdgrp lng="cs">
    <trd>jednoplo&scaron;n&iacute;k</trd>,
    <trd>monopl&aacute;n</trd>
  </trdgrp>
  <trd lng="de">Eindecker</trd>
  <trd lng="en">monoplane</trd>
  <trd lng="es">monoplano</trd>
  <trdgrp lng="hu">
    <trd>egyfedel&udblac; rep&uuml;l&odblac;g&eacute;p</trd>,
    <trd>monopl&aacute;n</trd>
  </trdgrp>
  <trd lng="it">monoplano</trd>
  <trd lng="nl">eendekker</trd>
  <trdgrp lng="pl">
    <trd>jednop&lstroke;atowiec</trd>,
    <trd>jednop&lstroke;at</trd>
  </trdgrp>
  <trd lng="pt">monoplano</trd>
  <trd lng="ru">&c_m;&c_o;&c_n;&c_o;&c_p;&c_l;&c_a;&c_n;</trd>
  <trd lng="sk">jednoplo&scaron;n&iacute;k</trd>
</drv>
</art>
