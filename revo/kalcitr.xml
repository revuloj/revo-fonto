<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kalcitr.xml,v 1.25 2025/03/31 16:20:09 revo Exp $">
<kap>
   <rad>kalcitr</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="kalcitr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kalcitr.0i.bati">
    <dif>
      <klr>(p.p. <ref cel="pied.kvar0ulo">kvarpiedulo</ref>)</klr>
      Bati per la postaj piedoj, <ref tip="super" cel="lev.0i.starigi">levante</ref> sin sur la anta&ubreve;aj:
      <ekz>
         la du &ccirc;evaloj <klr>[&#8230;]</klr> <tld/>is unu 
         kontra&ubreve; la alia
         <fnt>
           <bib>RugxDom</bib>, <lok>&ccirc;apitro 65a, volumo 2a, p. 406a</lok>
         </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la &scirc;vabrilo <klr>[&#8230;]</klr> per la ekstremo de la tenilo &gcirc;i tamburis ruladon sur
        la planko, &gcirc;i <tld/>is kaj strebis al la fenestro, <nom>Margarita</nom> ravite jelpis
        kaj salte ekrajdis la balailon
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
     </dif>
     <ref tip="ant" cel="bauxm.0i.RAJ">ba&ubreve;mi<sncref/></ref>
     <refgrp tip="vid">
       <ref cel="bat.hufo0i">hufobati</ref>,
       <ref cel="karako.0i">karakoli</ref>,
       <ref cel="pranc.0i.RAJ">pranci</ref>,
       <ref cel="stamf.0i">stamfi</ref>
     </refgrp>
     <trd lng="be">&c_b;&c_r;&c_y;&c_k;&c_a;&c_c;&c_c;&c_a;</trd>
     <trd lng="ca">guitar</trd>
     <trdgrp lng="de">
       <trd>bocken <klr>(Pferd)</klr></trd>,
       <trd>buckeln <klr>(Pferd)</klr></trd>,
       <trd>ausschlagen</trd>,
       <trd>sich auf die <ind>Vorderbeine</ind> stellen</trd>
     </trdgrp>
     <trd lng="en">buck</trd>
     <trd lng="es">cocear</trd>
     <trdgrp lng="fi">
       <trd>potkia<klr tip="amb"> (takajaloillaan)</klr></trd>,
     </trdgrp>
     <trd lng="hu">kir&uacute;g<klr tip="ind"> (l&oacute;)</klr></trd>
     <trd lng="nl">achteruitschoppen</trd>
     <trd lng="pt">recalcitrar</trd>
     <trdgrp lng="ru">
       <trd>&c_l;&c_ja;&c_g;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
       <trd>&c_b;&c_r;&c_y;&c_k;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
     </trdgrp>
  </snc>
  <snc mrk="kalcitr.0i.ribeli">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Energie <ref tip="super" cel="ribel.0i">ribeli</ref> kontra&ubreve; ia trudo:
      <ekz>
         necesas, ke mi submeti&gcirc;u al <klr>[mia destino]</klr>
         sen rezonadi kaj <tld/>i, &ccirc;ar tio estas senutila
         <fnt><bib>Revadoj</bib><lok>Oka promenado</lok></fnt>;
      </ekz>
      <ekz>
        la najbaroj (Pollando kaj Litovio) oponas kaj okcidentaj (eks)potencoj <tld/>as
        <fnt>
          <bib>LOdE</bib>
          <lok>1999,  No 6 (56)</lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru 
      <ke>kontra&#365;star(eg)i</ke>.
    </rim>    
     <trdgrp lng="be">
       <trd>&c_a;&c_d;&c_b;&c_ib;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
       <trd>&c_s;&c_u;&c_p;&c_r;&c_a;&c_c;&c_mol;&c_s;&c_t;&c_a;&c_ja;&c_c;&c_mol;</trd>
     </trdgrp>
     <trdgrp lng="ca">
       <trd>recalcitrar</trd>,
       <trd>combatre</trd>,
       <trd>opugnar</trd>
     </trdgrp>
     <trdgrp lng="de">
       <trd>sich <ind>auflehnen</ind></trd>,
       <trd>rebellieren</trd>
     </trdgrp><trdgrp lng="fi">
       <trd>panna vastaan</trd>,
       <trd>rimpuilla</trd>
     </trdgrp>
     <trdgrp lng="nl">
       <trd>tegenspartelen</trd>,
       <trd>tegenstribbelen</trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>&c_o;&c_t;&c_b;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
       <trd>&c_o;&c_t;&c_b;&c_r;&c_y;&c_k;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
     </trdgrp>
     <trdgrp lng="zh">
      <trd>&#21467;&#36870;&#30340; <pr>p&#224;nn&#236;de</pr></trd>,
      <trd>&#21467;&#36870; <pr>p&#224;nn&#236;</pr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>jan&ccaron;it</trd>,
    <trd>kopat</trd>,
    <trd>vyhazovat <klr>(o koni)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
     <trd>ruer</trd>,
     <trd>s'<ind>insurger</ind></trd>,
     <trd>se <ind>rebiffer</ind></trd>,
     <trd>regimber</trd>,
     <trd>ruer dans les brancards</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#24460;&#33050;&#12391;&#36468;&#19978;&#12370;&#12427;  <pr>&#12398;&#12385;&#12354;&#12375;&#12391;&#12369;&#12354;&#12370;&#12427;</pr></trd>,
    <trd>&#38929;&#24375;&#12395;&#25269;&#25239;&#12377;&#12427;  <pr>&#12364;&#12435;&#12365;&#12423;&#12358;&#12395;&#12390;&#12356;&#12371;&#12358;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
     <trd>krn&aogonek;brnym by&cacute;</trd>,
     <trd>opiera&cacute; si&eogonek;</trd>,
     <trd>wierzga&cacute;</trd>,
     <trd>wzbrania&cacute; si&eogonek;</trd>
  </trdgrp>
  <trd lng="ro">fie recalcitrant</trd> 
  <trdgrp lng="sk">
    <trd>jan&ccaron;i&tcaron;</trd>,
    <trd>kopa&tcaron;<klr>(o ko&ncaron;ovi)</klr></trd>,
    <trd>prie&ccaron;i&tcaron; sa</trd>,
    <trd>vyhadzova&tcaron; zadn&yacute;mi nohami</trd>,
    <trd>vzoprie&tcaron; sa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_r;&c_i;&c_k;&c_a;&c_t;&c_i;(&c_s;&c_ja;)</trd>,
    <trd>&c_h;&c_v;&c_i;&c_c;&c_a;&c_t;&c_i;(&c_s;&c_ja;)</trd>
  </trdgrp>
</drv>

<drv mrk="kalcitr.0o">
   <kap><tld/>o</kap>
   <snc>
     <dif>
       Ago <tld/>i.
     </dif>
   </snc>
   <trd lng="be">&c_b;&c_r;&c_y;&c_k;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
   <trdgrp lng="ca">
     <trd>guitza</trd>,
     <trd>resist&egrave;ncia</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>Bocken <klr>(Pferd)</klr></trd>,
     <trd>Auflehnung</trd>
   </trdgrp><trd lng="es">coz</trd>
   <trdgrp lng="fr">
     <trd>ruade</trd>,
     <trd>regimbement</trd>
   </trdgrp>
   <trd lng="hu">kir&uacute;g&aacute;s <klr tip="ind">(l&oacute;&eacute;)</klr></trd>
   <trdgrp lng="ja">
    <trd>&#24460;&#33050;&#12391;&#36468;&#19978;&#12370;&#12427;&#12371;&#12392;  <pr>&#12398;&#12385;&#12354;&#12375;&#12391;&#12369;&#12354;&#12370;&#12427;&#12371;&#12392;</pr></trd>,
    <trd>&#38929;&#24375;&#12394;&#25269;&#25239;  <pr>&#12364;&#12435;&#12365;&#12423;&#12358;&#12394;&#12390;&#12356;&#12371;&#12358;</pr></trd>,
    <trd>&#21453;&#25239;  <pr>&#12399;&#12435;&#12371;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">schop achteruit</trd>
   <trd lng="pl">wierzganie</trd>     
   <trd lng="ro">lovituri cu piciorul</trd>
   <trdgrp lng="ru">
     <trd>&c_b;&c_r;&c_y;&c_k;&c_a;&c_n;&c_i;&c_je;</trd>,
     <trd>&c_l;&c_ja;&c_g;&c_a;&c_n;&c_i;&c_je;</trd>
   </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_r;&c_i;&c_k;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_h;&c_v;&c_i;&c_c;&c_a;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
  <trd lng="zh">&#21467;&#20081; <pr>p&#224;nlu&#224;n</pr></trd>
</drv>
</art>
