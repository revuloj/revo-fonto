<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: nankin.xml,v 1.8 2023/09/02 12:44:20 afido Exp $">
    <kap>
      <rad>Nankin</rad>/o
    </kap>

<drv mrk="nankin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="nankin.0o.urbo">
    <dif>
      Granda <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref> en la oriento 
      de <ref tip="malprt" cel="cxin.CX0ujo">&Ccirc;inujo</ref>
      &ccirc;e la komenco de la <ref cel="jangzi.0o">Jangzi</ref>-delto, do nordokcidente 
      de <ref cel="sxanhaj.SX0o">&Scirc;anhajo</ref>:
      <ekz>
        <tld/>o havas historion de pli ol 2000 jaroj, ekde 220 &gcirc;is 589 &gcirc;i estis &ccirc;efurbo
        de malgrandaj dinastioj, ekde 1127 &gcirc;is 1279 &ccirc;efurbo de la suda Dinastio <nom>Song</nom>,
        kaj komence de la Dinastio <nom>Ming</nom> <tld/>o estis &ccirc;efurbo de la tuta &Ccirc;inio
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Nankino">Nankino</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en grandaj urboj kiaj Pekino, <tld/>o kaj Kantono, sankciite de la lokaj
        registaroj, sportaj institucioj grandkvante proponas loteriajn biletojn
        <fnt><aut>Mu Binghua</aut>,
        <vrk><url ref="&Monato;/2000/006920.php">Loterioj</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/06, p. 13</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Nanjing</trd>,
      <trd>Nanking</trd>
    </trdgrp>
    <trd lng="en">Nanjing</trd>
    <trd lng="fr">Nankin</trd>
    <trd lng="nl">Nanjing</trd>
    <trd lng="pl">Nankin</trd>
    <trd lng="ro">Nanjing </trd>
    <trd lng="zh">&#x5357;&#x4EAC;</trd>
  </snc>
  <ref tip="hom" cel="nanken.0o">nankeno</ref>
  <trdgrp lng="be">
    <trd>&c_N;&c_a;&c_n;&c_k;&c_ib;&c_n;</trd>,
    <trd>&c_N;&c_a;&c_n;&c_mol;&c_c;&c_z;&c_ib;&c_n;</trd>
  </trdgrp>
  <trd lng="ja">&#x30CA;&#x30F3;&#x30AD;&#x30F3;</trd>
  <trd lng="uk">&#x41D;&c_a;&#x43D;&#x43A;&c_ib;&#x43D;</trd>
</drv>

  </art>

