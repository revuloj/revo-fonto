<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dusx.xml,v 1.27 2022/04/26 17:11:48 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>du&scirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="dusx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="dusx.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="sxpruc.0igi">&Scirc;prucigi</ref> akvon sur iun:
      <ekz>
        &scirc;i gvidis min al mia &ccirc;ambro <klr>[&#x2026;]</klr> kaj, <tld/>inte min per &ccirc;iela rideto,
        foriris
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        tra la fenestro vidi&gcirc;is pluvo<tld/>ataj domoj kaj pluvolavataj stratoj
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        mia patro nur post <tld/>ado odoras je sia kolonja akvo
        <fnt>
          <aut>Spomenka &Scaron;timec</aut>
          <vrk>Ombro sur interna pejza&gcirc;o</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="asperg.0i.sxpruci">aspergi<sncref/></ref>,
      <ref cel="akv.0umi">akvumi</ref>,
      <ref cel="versx.0i">ver&scirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="de">duschen</trd>
  <trd lng="en">shower</trd>
  <trd lng="es">duchar</trd>
  <trd lng="fa">&a_z;&f_y;&a_r; <ind>&a_d;&a_w;&a_sh;</ind> &a_b;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">doucher</trd>
  <trd lng="he">&lamed;&resh;&het;&vav;&ftsadi;
  \<ind>&resh;&het;&ftsadi;</ind>\</trd>
  <trdgrp lng="hu">
    <trd>zuhanyoz</trd>,
    <trd>letusol</trd>
  </trdgrp>
  <trd lng="nl">besprenkelen</trd>
  <trdgrp lng="pl">
    <trd>bra&cacute; <ind>prysznic</ind></trd>,
    <trd>k&aogonek;pa&cacute; pod <ind>prysznicem</ind></trd>,
    <trd>bra&cacute; <ind>natrysk</ind></trd>,
    <trd>k&aogonek;pa&cacute; pod <ind>natryskiem</ind></trd>,
    <trd>bra&cacute; <ind>dusz</ind><klr> (rzadkie)</klr></trd>
  </trdgrp>
  <trd lng="sk">sprchova&tcaron;</trd>
  <trd lng="sv">duscha<klr> (n&aring;gon eller n&aring;got)</klr></trd>
  <trd lng="uk">&#x440;&#x43E;&#x431;&#x438;&#x442;&#x438; &#x43E;&#x431;&#x43B;&#x438;&#x432;&#x430;&#x43D;&#x43D;&#x44F;</trd>
</drv>

<drv mrk="dusx.0o">
  <kap><tld/>o</kap>
  <snc mrk="dusx.0o.KOMUNE">
    <dif>
      &Scirc;prucigo de akvo sur iun parton de la korpo:
      <ekz>
        vi devus &scirc;an&gcirc;i viajn vestojn, ili estas tute malsekaj <klr>[&#x2026;]</klr>, li estis
        tute forgesinta la malvarmegan <tld/>on <klr>(maltroige: akvover&scirc;on)</klr>, kiu falis sur lin anta&ubreve;
        duono da horo
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la ekhalto <klr>[&#x2026;]</klr> anta&ubreve; <nom>Li&egrave;ge</nom> estis al li kvaza&ubreve; glacie malvarma
        <tld/>o, ili devus esti en Parizo je tiu tempo
        <fnt>
          <bib>HsT</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_d;&c_u;&c_sh;</trd>
    <trd lng="bg">&c_d;&c_u;&c_sh;</trd>
    <trd lng="en">shower</trd>
    <trd lng="es">ducha</trd>
    <trd lng="fa">&a_d;&a_w;&a_sh;<klr tip="ind"> (&a_H;&a_m;&a_A;&a_m;)</klr></trd>
    <trd lng="fr">douche<klr> (jet d'eau)</klr></trd>
    <trd lng="he">&mem;&qof;&lamed;&het;&tav;</trd>
    <trdgrp lng="hu">
      <trd>zuhany</trd>,
      <trd>tus<klr tip="ind"> (zuhany)</klr></trd>
    </trdgrp>
    <trd lng="nl">douche</trd>
    <trdgrp lng="pl">
      <trd>prysznic</trd>,
      <trd>natrysk</trd>,
      <trd>dusz<klr> (rzadkie)</klr></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>ducha</trd>,
      <trd>banho de chuveiro</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_u;&c_sh;</trd>,
      <trd>&c_o;&c_b;&c_l;&c_i;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
    <trd lng="sk">sprcha</trd>
    <trd lng="sv">dusch</trd>
  </snc>
  <snc mrk="dusx.0o.ilo">
    <dif>
      <tld lit="D"/>ilo:
      <ekz>
        mia dommastro senvestigis sin anta&ubreve; mi sen ia prudeco, ekzemplo, kiun devis
        sekvi anka&ubreve; mi por puri&gcirc;i sub la <tld/>o en la apuda ban&ccirc;ambro
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        traktado per fre&scirc;a akvo el la <tld/>o de la ban&ccirc;ambro revigligas ilin
        <fnt>
          <bib>ViV</bib>
        </fnt>;
      </ekz>
      <ekz>
        iu insida satano &scirc;telis sin en la <tld/>tubon, la <tld/>o cetere ne funkciis jam
        de kvin jaroj
        <fnt>
          <bib>LGA</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Dusche</trd>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x431;&#x43B;&#x438;&#x432;&#x430;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x434;&#x443;&#x448; (&#x434;&#x456;&#x44F;)</trd>
  </trdgrp>
</drv>

<drv mrk="dusx.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="dusx.0ejo.KOMUNE">
    <dif>
      Loko aran&gcirc;ita por <tld/>o:
      <ekz>
        la &ccirc;ambroj de la hoteloj havas <klr>[&#x2026;]</klr> 
        <tld/>ejon
        <fnt>
          <aut>-</aut>,
          <vrk>Utilaj informoj pri Hanojo kaj Vjetnamio</vrk>,
          <lok><url ref="&CRI;other/magazine/dua_bulteno_2012.pdf">
          Universala Kongreso de Esperanto, Dua Bulteno,
          2012</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ar &ccirc;iuj unuopaj &ccirc;eloj estas okupitaj, ili akceptas komunan <tld/>ejon
        <fnt>
          <bib>KrB</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Duschkabine</trd>,
    <trd>Duschzelle</trd>
  </trdgrp>
  <trd lng="fr">douche<klr> (lieu)</klr></trd>
  <trd lng="uk">&#x434;&#x443;&#x448;&#x43E;&#x432;&#x430;</trd>
</drv>

<drv mrk="dusx.sin0i">
  <kap><tld/>i&gcirc;i, <var><kap>sin <tld/>i</kap></var></kap>
  <snc mrk="dusx.sin0i.KOMUNE">
    <dif>
      Sin <ref tip="super" cel="lav.0i">lavi</ref> per <tld/>o:
      <ekz>
        &scirc;i esprimis du dezirojn: man&gcirc;i finfine fre&scirc;an miksitan salaton kaj <tld/>i&gcirc;i
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2015/011521.php">Festanta la revenon de Astrosamantha</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_m;&c_a;&c_c;&c_mol;
  <ind>&c_d;&c_u;&c_sh;</ind></trd>
  <trdgrp lng="de">
    <trd>sich <ind>duschen</ind></trd>,
    <trd>eine <ind>Dusche</ind> nehmen</trd>
  </trdgrp>
  <trd lng="en">take a <ind>shower</ind></trd>
  <trd lng="es">ducharse</trd>
  <trdgrp lng="fa">
    <trd>&a_d;&a_w;&a_sh; &f_g;&a_r;&a_f;&a_t;&a_n;</trd>,
    <trd>&a_z;&f_y;&a_r; <ind>&a_d;&a_w;&a_sh;</ind> &a_r;&a_f;&a_t;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>prendre une <ind>douche</ind></trd>,
    <trd>se <ind>doucher</ind></trd>
  </trdgrp>
  <trd lng="he">&lamed;&he;&tav;<ind>&qof;&lamed;&het;</ind></trd>
  <trdgrp lng="hu">
    <trd>zuhanyozik</trd>,
    <trd>tusol</trd>
  </trdgrp>
  <trd lng="nl">douchen</trd>
</drv>
</art>
