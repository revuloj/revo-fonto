<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: siluet.xml,v 1.25 2023/09/03 18:39:30 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>siluet</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="siluet.0o">
  <kap><tld/>o</kap>
  <snc mrk="siluet.0o.KOMUNE">
    <dif>
      <ref cel="form.0o.aspekto">Formo</ref> klare kontrasta al la &ccirc;irka&ubreve;a fono
      kaj kies propra enhavo malpli klaras;
      tiaspeca desegno de <ref cel="profil.0o.vizagxo">profila</ref> viza&gcirc;o, 
      de homo, de a&jcirc;o:
      <ekz>
        piramidoj estis distingeblaj kiel <tld/>oj en la <klr>[&#x2026;]</klr> dezerto
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        li e&ccirc; ne vidis la virinan <tld/>on malanta&ubreve; la vira figuro
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        lia eleganta kaj gracia <tld/>o, lia profilo kun delikataj brovoj kaj kun
        maldikaj lipoj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        en neklara nebulo <klr>[&#x2026;]</klr> estis videblaj nur <tld/>oj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la pluvego penetris &gcirc;is la ostoj <klr>[&#x2026;]</klr> mi vagis &ccirc;irka&ubreve; senbrue
        movi&gcirc;antaj <tld/>oj, stumblante
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili haltis sub la subiranta suno, forminte du kontrastajn <tld/>ojn
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la firmamento i&gcirc;is violeta, la nigraj <tld/>oj de la cipresoj fari&gcirc;is ankora&ubreve;
        pli klaraj, ol dum la taga lumo
        <fnt>
          <bib>QuV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kontur.0o.BELA">konturo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_l;&c_ju;&c_e;&c_t;</trd>
  <trd lng="ca">silueta</trd>
  <trdgrp lng="cs">
    <trd>kontura</trd>,
    <trd>obrys</trd>,
    <trd>silueta</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Umriss</trd>,
    <trd>Silhouette</trd>
  </trdgrp>
  <trd lng="en">silhouette</trd>
  <trdgrp lng="fa">
    <trd>&a_D;&a_d;&a_shadda;&a_n;&a_w;&a_r;</trd>,
    <trd>&a_s;&f_y;&a_l;&a_w;&a_y_hamza;&a_t;</trd>
  </trdgrp>
  <trd lng="fr">silhouette</trd>
  <trdgrp lng="it">
    <trd>profilo<klr> (figura)</klr></trd>,
    <trd>sagoma<klr> (disegno)</klr></trd>, 
    <trd>silhouette</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30B7;&#x30EB;&#x30A8;&#x30C3;&#x30C8;</trd>,
    <trd>&#x8F2A;&#x90ED;  [<ind>&#x308A;&#x3093;&#x304B;&#x304F;</ind>]</trd>,
    <trd>&#x5F71;&#x6CD5;&#x5E2B;  [<ind>&#x304B;&#x3052;&#x307C;&#x3046;&#x3057;</ind>]</trd>,
    <trd>&#x5F71;&#x7D75;  [<ind>&#x304B;&#x3052;&#x3048;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">silhouet</trd>
  <trdgrp lng="hu">
    <trd>&aacute;rnyk&eacute;p</trd>,
    <trd>&aacute;rnyrajz</trd>,
    <trd>k&ouml;rvonal</trd>,
    <trd>sziluett</trd>
  </trdgrp>
  <trd lng="pt">silhueta</trd>
  <trd lng="ru">&c_s;&c_i;&c_l;&c_u;&c_e;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>obrys</trd>,
    <trd>silueta</trd>
  </trdgrp>
  <trd lng="uk">&#x441;&#x438;&#x43B;&#x443;&#x435;&#x442;</trd>
</drv>

<drv mrk="siluet.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="siluet.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="aper.0igi.vidigi">Aperigi</ref> kiel <tld/>o:
      <ekz>
        steloj post steloj <klr>[&#x2026;]</klr> malaperis trans neklare <tld/>ita horizonto
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        La tuto tiujn <tld/>as, // Kiuj verslibrojn ne a&ccirc;etas
        <fnt>
          <bib>LSB</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="star.el0igi">elstarigi</ref>
  </snc>
</drv>

<drv mrk="siluet.0igxi">
  <kap>
    <tld/>i&gcirc;i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="siluet.0igxi.KOMUNE">
    <dif>
      <ref cel="aper.0i.sinVidigi">Aperi</ref> <tld/>e:
      <ekz>
        la loga surdula mondo, eble &gcirc;is nun nekonata al la plimulto, <tld/>i&gcirc;os &ccirc;e la
        ekrano de via komputilo
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011278.php">Vortaro intuicia, horizonto internacia</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">sich als Silhouette <ind>abheben</ind></trd>
  <trd lng="fr">se <ind>d&eacute;couper</ind> sur <klr>(en
  silhouette)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x30B7;&#x30EB;&#x30A8;&#x30C3;&#x30C8;&#x304C;&#x6D6E;&#x304B;&#x3076;  [<ind>&#x3057;&#x308B;&#x3048;&#x3063;&#x3068;&#x304C;&#x3046;&#x304B;&#x3076;</ind>]</trd>,
    <trd>&#x8F2A;&#x90ED;&#x304C;&#x6620;&#x308B;  [<ind>&#x308A;&#x3093;&#x304B;&#x304F;&#x304C;&#x3046;&#x3064;&#x308B;</ind>]</trd>
  </trdgrp>
</drv>



</art>
