<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: murb.xml,v 1.2 2026/01/18 18:00:21 afido Exp $">
    <kap>
      <rad>murb</rad>/o
      <fnt><bib>PIV2</bib></fnt>
    </kap>

<drv mrk="murb.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Knedebla grasri&ccirc;a <ref tip="super" cel="past.0o.KUI">pasto</ref>,
      kiu bakata nemulte &scirc;velas kaj
      el kiu oni faras keksojn, tortetojn k.a.:
      <ekz>
        ki&scirc;o <klr>[el]</klr> <tld/>o, fuma&jcirc;ita lardo, ov-laktokrema&jcirc;o
        <fnt>
          <vrk><url ref="https://gresillon.org/spip.php?page=article&amp;id_article=102"
              >Menuoj dum sta&gcirc;oj, 2012, vesperman&gcirc;o</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[por]</klr> poma torto <klr>[&#8230;]</klr> vi bezonos kompoton, <tld/>on,
        <klr>[&#8230;]</klr> pomojn kaj spicojn
        <fnt>
          <vrk><url ref="https://apetito.ikso.net/ingredienco/pomoj"
              >Lasta recepto kun pomoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">M&uuml;rbeteig</trd>
    <trd lng="en">shortcrust pastry</trd>
    <trd lng="fr">p&acirc;te bris&eacute;e</trd>
    <trd lng="ru">&c_p;&c_je;&c_s;&c_o;&c_ch;&c_n;&c_o;&c_je; &c_t;&c_je;&c_s;&c_t;&c_o;</trd>
  </snc>
  <trdgrp lng="ca">
    <trd>pasta trencada</trd>,
    <trd>pasta brisa</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>masa quebrada</trd>,
    <trd>pasta brisa</trd>
  </trdgrp>
</drv>

  </art>

