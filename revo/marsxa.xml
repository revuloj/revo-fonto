<art mrk="$Id: marsxa.xml,v 1.23 2024/01/25 20:10:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>mar&scirc;al</rad>/o
</kap>
<drv mrk="marsxa.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="marsxa.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Plej altranga <ref cel="oficir.0o.MIL">oficiro</ref>
      en iuj landoj.
    </dif>
    <trd lng="fr">mar&eacute;chal<klr tip="ind"> (officier de plus haut
    grade)</klr></trd>
    <trdgrp lng="hu">
      <trd>t&aacute;bornagy</trd>,
      <trd>marsall</trd>
    </trdgrp>
  <trd lng="it">maresciallo<klr> (ufficiale generale)</klr></trd>
  <trd lng="pl">marsza&lstroke;ek</trd>
  <trd lng="pt">marechal <klr>(mil.)</klr></trd>
  </snc>
  <snc mrk="marsxa.0o.civila">
    <dif>
      Titolo de diversaj kortegaj a&ubreve; &scirc;tataj altranguloj
      en iuj landoj:
      <ekz>
        la oficiala programo anta&ubreve;vidas <klr>[...]</klr> kvarokulajn 
        interparolojn de la Sejm<tld/>o <klr>(prezidanto de la pola 
        Sejmo)</klr> kun la prezidanto de Bundestag
        <fnt>
        <vrk>Renkonti&gcirc;o de polaj kaj germanaj deputitoj</vrk>
        <lok><url ref="http://www.radio.com.pl/polonia/article.asp?tId=15674">Pola
        Radio (2004-10-18)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>mar&eacute;chal<klr tip="ind"> (non militaire)</klr></trd>,
      <trd>pr&eacute;sident</trd>
    </trdgrp>
    <trd lng="hu">marsall</trd>
    <trd lng="it">maresciallo<klr> (non militare)</klr></trd>
    <trd lng="pl">marsza&lstroke;ek Sejmu</trd> 
  <trdgrp lng="pt">
    <trd>marechal <klr>(civil)</klr></trd>,
    <trd>presidente</trd>
  </trdgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_sh;&c_a;&c_l;</trd>
  <trd lng="bg">&c_m;&c_a;&c_r;&c_sh;&c_a;&c_l;</trd>
  <trd lng="br">marichal</trd>
  <trd lng="de">Marschall</trd>
  <trd lng="en">marshal</trd>
  <trd lng="es">Mariscal</trd>
  <trdgrp lng="ja">
    <trd>&#x5143;&#x5E25;  <pr>&#x3052;&#x3093;&#x3059;&#x3044;</pr></trd>,
    <trd>&#x9AD8;&#x5B98;  <pr>&#x3053;&#x3046;&#x304B;&#x3093;</pr></trd>,
    <trd>&#x56FD;&#x4F1A;&#x8B70;&#x9577;  <pr>&#x3053;&#x3063;&#x304B;&#x3044;&#x304E;&#x3061;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">maarschalk</trd>
  <trd lng="pl">marsza&lstroke;ek</trd>     
  <trd lng="ro">mare&#x219;al</trd>
  <trd lng="ru">&c_m;&c_a;&c_r;&c_sh;&c_a;&c_l;</trd>
  <trd lng="uk">&#x43C;&#x430;&#x440;&#x448;&#x430;&#x43B;</trd>
  <trdgrp lng="zh">
    <trd>&#x5143;&#x5E05; <pr>yu&#xE1;nshu&#xE0;i</pr></trd>,
    <trd>&#x5143;&#x5E25; <pr>yu&#xE1;nshu&#xE0;i</pr></trd>,
    <trd>&#x99AC;&#x6B47;&#x723E; <pr>m&#x1CE;xi&#x113;&#x11B;r</pr></trd>,
    <trd>&#x9A6C;&#x6B47;&#x5C14; <pr>m&#x1CE;xi&#x113;&#x11B;r</pr></trd>
  </trdgrp>
</drv>
