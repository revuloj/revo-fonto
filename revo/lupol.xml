<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lupol</rad>/o
</kap>
<drv mrk="lupol.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio el familio <ref tip="super" cel="kanaba.0oj">kanabacoj</ref>
      (<trd lng="la">Humulus lupulus</trd>),
      grimpanta vegeta&jcirc;o, kies femalaj amaraj floroj estas
      uzataj por aromigi la <ref tip="malprt" cel="bier.0o">bieron</ref>.
    </dif>
  </snc>
  <bld lok="../bld/lupol.jpg">folioj kaj fruktokonusoj</bld>
  <trd lng="de">Hopfen</trd>
  <trd lng="nl">hop</trd>
  <trd lng="ru">&c_h;&c_m;&c_je;&c_l;&c_mol; <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
</drv>
</art>
