<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>lupol</rad>/o
</kap>
<drv mrk="lupol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio el familio <ref tip="super" cel="kanab.0acoj">kanabacoj</ref>
      (<trd lng="la">Humulus lupulus</trd>),
      grimpanta vegeta&jcirc;o, kies femalaj amaraj floroj estas
      uzataj por aromigi la <ref tip="malprt" cel="bier.0o">bieron</ref>.
    </dif>
  </snc>
  <bld lok="../bld/lupol.jpg">folioj kaj fruktokonusoj</bld>
  <trd lng="de">Hopfen</trd>
</drv>
</art>
