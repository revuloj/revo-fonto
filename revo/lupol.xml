<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>lupol</rad>/o
</kap>
<drv mrk="lupol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio el familio <ref tip="super" cel="kanab.0acoj">kanabacoj</ref>
      (<trd lng="la">Humulus lupulus</trd>), 
      grimpanta vegeta&jcirc;o, kies femalaj amaraj floroj estas uzataj
      por aromigi la bieron.
    </dif>
  </snc>
  <trd lng="de">Hopfen</trd>
</drv>
</art>
