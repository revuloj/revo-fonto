<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lupol.xml,v 1.23 2020/02/15 23:10:54 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lupol</rad>/o
</kap>

<drv mrk="lupol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio el familio <ref tip="super"
      cel="kanaba.0oj">kanabacoj</ref>
      (<trd lng="la">Humulus lupulus</trd>),
      grimpanta vegeta&jcirc;o, kies femalaj amaraj floroj estas
      uzataj por aromigi la <ref tip="malprt"
      cel="bier.0o">bieron</ref>:
      <ekz>
        &ccirc;e la bariloj volvi&gcirc;adis supren sova&gcirc;a <tld/>o kaj florantaj
        konvolvuloj
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o &ccirc;arme odoris
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de la jaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        sian bierkru&ccirc;on li &ccirc;irka&ubreve;volvis per brune-verdaj volvotrunkoj de
        <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Dekdu per la po&scirc;to</lok>
        </fnt>;
      </ekz>
      <ekz>
        kune kun <tld/>o, la malalta temperaturo bremsas la bakterian
        kreskadon, pro kio la biero i&gcirc;as pli klara<fnt><aut>Lode Van De
        Velde</aut>: <vrk>Bierlando Belgio</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 1a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <bld
     lok="&WCU;/thumb/e/eb/Hopfen3.jpg/640px-Hopfen3.jpg"
     prm="CC BY-SA 2.5"
     >fruktokonusoj de sova&gcirc;a <tld/>o (Humulus lupulus)
       <fnt>
         <aut>Dr. Hagen Graebner, propra foto</aut>
         <lok><url ref="&WCW;/File:Hopfen3.jpg"
             >&WCW;/File:Hopfen3.jpg</url></lok>
       </fnt>
  </bld>
  <trd lng="be">&c_h;&c_m;&c_je;&c_l;&c_mol;</trd>     
  <trd lng="bg">&c_h;&c_m;&c_je;&c_l;</trd>
  <trd lng="br">houpez(enn)</trd>
  <trd lng="de">Hopfen</trd>
  <trd lng="en">hop</trd>  
  <trd lng="es">l&uacute;pulo</trd> 
  <trd lng="fa">&a_r;&a_A;&a_z;&f_k;</trd>
  <trd lng="fr">houblon</trd>
  <trd lng="hu">koml&oacute;</trd>
  <trd lng="nl">hop</trd>
  <trd lng="pl">chmiel</trd> 
  <trd lng="pt">l&uacute;pulo</trd>
  <trd lng="ro">hamei</trd>
  <trd lng="ru">&c_h;&c_m;&c_je;&c_l;&c_mol; <klr
  tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="sv">humle</trd>
  <trd lng="sk">chme&lcaron;</trd>
  <trd lng="cs">chmel ot&aacute;&ccaron;iv&yacute;</trd>
</drv>
</art>
