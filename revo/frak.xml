<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: frak.xml,v 1.24 2025/10/04 08:32:04 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>frak</rad>/o
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="frak.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Vira ceremonia <ref tip="super" cel="vest.super0o">supervesto</ref>
      kun du mallar&gcirc;aj postaj <ref tip="prt" cel="bask.0o.vestoparto">basko</ref>j:
      <ekz>
        en <tld/>o kaj kun cilindra &ccirc;apelo
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 21</lok>
        </fnt>;
      </ekz>
      <ekz>
        eniris homo sesdekjara <klr>[&#x2026;]</klr> en ka&scirc;tankolora
        <tld/>o kun kupraj butonoj
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 4a, p.
        17a</lok></fnt>;
      </ekz>
      <ekz>
        lia <tld/>o estas el silko, sed estas ne eble diri precize, kian koloron &gcirc;i havas
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;mi helpos al vi ekstari&rightquot;, <klr>[&#x2026;]</klr> kaj kaptante lin &ccirc;e la kolumo de lia
        <tld/>o, li starigis lin
        <fnt>
          <bib>IdO</bib>
          <lok>Kvara Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="jak.0o">jako</ref>,
      <ref cel="jxaket.0o">&jcirc;aketo</ref>,
      <ref cel="reding.0o">redingoto</ref>,
      <ref cel="surtut.0o">surtuto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_f;&c_r;&c_a;&c_k;</trd>
  <trd lng="bg">&c_f;&c_r;&c_a;&c_k;</trd>
  <trd lng="cs">frak</trd>
  <trdgrp lng="de">
    <trd>Frack</trd>,
    <trd>Schwalbenschwanz <klr>(Frack)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>dress coat</trd>,
    <trd>tailcoat</trd>,
    <trd>tails</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>frac</trd>,
    <trd>queue-de-morue<klr> (frac)</klr></trd>,
    <trd>queue-de-pie<klr> (frac)</klr></trd>
  </trdgrp><trd lng="fr">frac</trd>
  <trd lng="hu">frakk</trd>
  <trd lng="it">frac</trd>
  <trd lng="ja">&#x71D5;&#x5C3E;&#x670D;  <pr>&#x3048;&#x3093;&#x3073;&#x3075;&#x304F;</pr></trd>
  <trd lng="nl">frak</trd>
  <trd lng="pl">frak</trd>
  <trdgrp lng="pt">
    <trd>fraque</trd>,
    <trd>casaca</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_r;&c_a;&c_k;</trd>
  <trd lng="sk">frak</trd>
  <trd lng="uk">&c_f;&c_r;&c_a;&c_k;</trd>
</drv>

<drv mrk="frak.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Homo per <tld/>o vestita:
      <ekz>
        turbana, mongol-viza&gcirc;a <tld/>ulo paroladas kun du damoj
        <fnt>
          <aut>Jen&odblac; Rejt&odblac;, trad. L&aacute;szl&oacute; Bal&aacute;zs</aut>
          <vrk><url ref="https://libro.ee/file.php?id=2887"
              >La Malbenita Bordo</url></vrk>
          <lok>&ccirc;ap. 4</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k; &c_u; &c_f;&c_r;&c_a;&c_k;&c_u;</trd>
  <trd lng="cs">&ccaron;l. oble&ccaron;en&yacute; do fraku</trd>
  <trdgrp lng="de">
    <trd>Fracktr&auml;ger</trd>,
    <trd>Befrackter</trd>
  </trdgrp>
  <trd lng="hu">frakkos ember</trd>
  <trd lng="it">uomo in <ind>frac</ind></trd>
  <trd lng="ja">&#x71D5;&#x5C3E;&#x670D;&#x3092;&#x7740;&#x305F;&#x4EBA;  <pr>&#x3048;&#x3093;&#x3073;&#x3075;&#x304F;&#x3092;&#x304D;&#x305F;&#x3058;&#x3093;</pr></trd>
  <trd lng="sk">&ccaron;lovek vo fraku</trd>
</drv>
</art>
