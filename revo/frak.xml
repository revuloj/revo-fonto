<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>frak</rad>/o
</kap>
<drv mrk="frak.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Vira ceremonia vesto kun du mallar&gcirc;aj postaj baskoj. 
    </dif>
  </snc>
  <trd lng="de">Frack</trd>
  <trd lng="ru">&c_f;&c_r;&c_a;&c_k;</trd>
</drv>
<drv mrk="frak.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Homo per <tld/>o vestita. 
    </dif>
    <refgrp tip="sin">
      <ref cel="jak.0o">jako</ref>,
      <ref cel="surtut.0o">surtuto</ref>,
      <ref cel="reding.0o">redingoto</ref>,
      <ref cel="jxaket.0o">&jcirc;aketo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Fracktr&auml;ger</trd>
</drv>
</art>
