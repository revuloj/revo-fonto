<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>frak</rad>/o
</kap>
<drv mrk="frak.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Vira ceremonia <ref tip="super"
      cel="vest.super0o">supervesto</ref>
      kun du mallar&gcirc;aj postaj baskoj:
      <ekz>
        eniris homo sesdekjara <klr>...</klr> en ka&scirc;tankolora 
        <tld/>o kun kupraj butonoj
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 4a, p. 17a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="jak.0o">jako</ref>,
      <ref cel="jxaket.0o">&jcirc;aketo</ref>,
      <ref cel="reding.0o">redingoto</ref>,
      <ref cel="surtut.0o">surtuto</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_f;&c_r;&c_a;&c_k;</trd>
  <trd lng="de">Frack</trd>
  <trdgrp lng="en">
    <trd>dress coat</trd>,
    <trd>tailcoat</trd>,
    <trd>tails</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>frac</trd>,
    <trd>queue-de-morue<klr> (frac)</klr></trd>,
    <trd>queue-de-pie<klr> (frac)</klr></trd>
  </trdgrp>
  <trd lng="fr">frac</trd>
  <trd lng="hu">frakk</trd>
  <trd lng="it">frac</trd>
  <trd lng="nl">frak</trd>
  <trd lng="pl">frak</trd>
  <trdgrp lng="pt">
    <trd>fraque</trd>,
    <trd>casaca</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_r;&c_a;&c_k;</trd>
</drv>

<drv mrk="frak.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Homo per <tld/>o vestita.
    </dif>
  </snc>
  <trd lng="de">Fracktr&auml;ger</trd>
  <trd lng="hu">frakkos ember</trd>
  <trd lng="it">uomo in <ind>frac</ind></trd>
</drv>
</art>
