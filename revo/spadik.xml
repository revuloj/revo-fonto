<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: spadik.xml,v 1.9 2023/10/04 22:27:27 revo Exp $">
<kap>
  <rad>spadik</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="spadik.0o">
  <kap><tld/>o</kap>
  <snc mrk="spadik.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="inflor.0o">Infloresko</ref>, kun
      longforma dika akso, sur kiu sidas la floroj,
      kune kun &ccirc;irka&ubreve;aj brakteoj:
      <ekz>
        Luka dume turnis al li la dorson kaj komencis
        senfoliigi verdajn maizajn <tld/>ojn
        <fnt>
          <aut>I. Silone, trad. D. Mistretta</aut>
          <vrk><url
          ref="http://ekladata.com/XiKWRW1ETT4zBOHrQZm16PxBQmc/silone-la-sekreto-de-luka.pdf">
          La sekreto de Luka</url></vrk>
          <lok>[vidita en 2018]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_ch;&c_a;&c_t;&c_a;&c_k; <klr tip="amb">(&c_s;&c_u;&c_k;&c_v;&c_je;&c_c;&c_mol;&c_c;&c_je;)</klr></trd>
  <trd lng="de">Kolben</trd>
  <trd lng="en">spadix</trd>
  <trd lng="fr">spadice</trd>
  <trd lng="hu">torzsa</trd>
  <trd lng="nl">kolf</trd>
  <trd lng="pl">kolba</trd>
  <trd lng="ru">&c_p;&c_o;&c_ch;&c_a;&c_t;&c_o;&c_k;</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_a;&c_ch;&c_a;&#x43D;</trd>,
    <trd>&#x43F;&#x43E;&c_ch;&c_a;&c_t;&#x43E;&#x43A;</trd>
  </trdgrp>
</drv>
</art>
