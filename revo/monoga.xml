<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>monogami</rad>/o
</kap>
<drv mrk="monoga.0o">
  <kap><tld/>o</kap>
  <snc mrk="monoga.0o.KOMUNE">
    <dif>
      Stato de viro havanta nur unu edzinon, a&ubreve; de virino
      havanta nur unu edzon.
    </dif>
    <trdgrp lng="pl">
      <trd>jedno&zdot;e&nacute;stwo</trd>,
      <trd>monogamia</trd>
    </trdgrp>
  </snc>
  <snc mrk="monoga.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Le&gcirc;a sistemo, la&ubreve; kiu viro ne povas havi pli ol unu
      edzinon,
      a&ubreve; edzino ne povas havi pli ol unu edzon.
    </dif>
    <trd lng="pl">monogamia</trd>
  </snc>
  <refgrp tip="vid">
    <ref cel="bigami.0o">bigamio</ref>,
    <ref cel="poliga.0o">poligamio</ref>.
  </refgrp>
  <trd lng="be">&c_m;&c_a;&c_n;&c_a;&c_g;&c_a;&c_m;&c_ib;&c_ja;</trd>
  <trd lng="de">Monogamie</trd>
  <trd lng="fr">monogamie</trd>
  <trd lng="hu">monog&aacute;mia</trd>
  <trd lng="nl">monogamie</trd>
  <trd lng="pt">monogamia</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_n;&c_o;&c_g;&c_a;&c_m;&c_i;&c_ja;</trd>,
    <trd>&c_je;&c_d;&c_i;&c_n;&c_o;&c_b;&c_r;&c_a;&c_ch;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sv">monogami</trd>
</drv>

<drv mrk="monoga.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Tiu, kiu vivas <tld/>e.
    </dif>
  </snc>
  <trd lng="de">Monogamist</trd>
  <trd lng="fr">monogame</trd>
  <trd lng="hu">monog&aacute;m</trd>
  <trdgrp lng="pl">
    <trd>monogamista<klr> {m}</klr></trd>,
    <trd>monogamistka<klr> {f}</klr></trd>
  </trdgrp>
</drv>
</art>
