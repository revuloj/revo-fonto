<vortaro>
<art mrk="$Id$">
<kap>
  <rad>dopamin</rad>/o
</kap>
<drv mrk="dopamin.0o">
  <kap><tld/>o</kap>
  <snc mrk="dopamin.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      <ref tip="super"
      cel="trans.nervo0igilo.FILZ">Nervotransigilo</ref>
      kun pluraj funkcioj: komando de muskoloj, kora 
      pulso, sangopremo...;
      simbole <frm>C<sub>8</sub>H<sub>11</sub>NO<sub>2</sub></frm>:
      <ekz>
        <tld/>o <klr>...</klr> estas ne&ubreve;rotransmisiilo
        <fnt>
          <aut>Viktoro</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/viktoro/319561">
          &Scirc;ablonado</url></vrk>,
          <lok>2011-04-07</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">dopamine</trd>
  <trd lng="fr">dopamine</trd>
</drv>
</art>

