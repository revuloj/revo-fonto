<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dopamin.xml,v 1.5 2023/11/04 18:50:51 revo Exp $">
<kap>
  <rad>dopamin</rad>/o
</kap>
<drv mrk="dopamin.0o">
  <kap><tld/>o</kap>
  <snc mrk="dopamin.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      <ref tip="super" cel="trans.nervo0igilo.FIZL">Nervotransigilo</ref>
      kun pluraj funkcioj: komando de muskoloj, kora 
      pulso, sangopremo...;
      simbole <frm>C<sub>8</sub>H<sub>11</sub>NO<sub>2</sub></frm>:
      <ekz>
        <tld/>o <klr>...</klr> estas ne&ubreve;rotransmisiilo
        <fnt>
          <aut>Viktoro</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/viktoro/319561">
          &Scirc;ablonado</url></vrk>,
          <lok>2011-04-07</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_p;&c_a;&c_m;&c_ib;&c_n;</trd>
  <trd lng="en">dopamine</trd>
  <trd lng="es">dopamina</trd>
  <trd lng="fr">dopamine</trd>
  <trd lng="zh">&#x591A;&#x5DF4;&#x80FA; <pr>du&#x14D;b&#x101;&#xE0;n</pr></trd>
</drv>
</art>

