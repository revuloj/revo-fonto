</kap>
<drv mrk="ornam.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ornam.0i.iu">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Aldoni al tuto plibeligajn detalojn:
      <ekz>
        <tld/>i tablon per
        floroj, <tld/>i &ccirc;ambron per pentra&jcirc;oj;
      </ekz>
      <ekz>
        <klr>[la]</klr> tombon <klr>[&#8230;]</klr> ili <tld/>is per folietoj de tilioj
        <fnt>
          <bib>Fab1</bib>
          <lok>Lekanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Eternulo <klr>[&#8230;]</klr> <tld/>as humilulojn per savo
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 149:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        iu el la plej junaj komizoj <tld/>is sian &ccirc;apelon per plumo
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ringofingron <klr>[&#8230;]</klr> <tld/>is sin per ora ringo
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; &scirc;tipo estos bela, se vi &gcirc;in <tld/>os
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        flag<tld/>i domon, &scirc;ipon.
      </ekz>
    </dif>
    <ref tip="sub" cel="drapir.0i">drapiri</ref>
  </snc>
  <snc mrk="ornam.0i.io">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Servi kiel plibeliga&jcirc;o:
      <ekz>
        cipreso, abio, kaj bukso kuni&gcirc;os, por <tld/>i la lokon
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 60:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        rozokolora atlaso kaj artefaritaj floroj <tld/>is &ccirc;iujn murojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        bonkora rideto <tld/>is &scirc;iajn dikajn lipojn
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        en aprilo tiom da violoj <tld/>as la herbaron, ke la verda&jcirc;o malaperas sub ili
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiun vesto <tld/>as, tiun homoj ekamas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        tablon <tld/>as ne tuko, sed kuko
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="dekora.0i">dekoracii</ref>,
    <ref cel="bel.pli0igi">plibeligi</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_u;&c_p;&c_r;&c_y;&c_g;&c_o;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_z;&c_d;&c_a;&c_b;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>b&yacute;t zdobn&yacute;</trd>,
    <trd>vyzdobit</trd>,
    <trd>zdobit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schm&uuml;cken</trd>,
    <trd>verzieren</trd>
  </trdgrp>
  <trd lng="el">&delta;&jota;&alfa;&sigma;&kappa;&omikron;&my;&omega_ton;</trd>
  <trd lng="en">adorn</trd>
  <trd lng="es">adornar</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;corer</trd>,
    <trd>enjoliver</trd>,
    <trd>orner</trd>,
    <trd>ornementer</trd>,
    <trd>parer</trd>
  </trdgrp>
  <trd lng="hu">d&iacute;sz&iacute;t</trd>
  <trdgrp lng="id">
    <trd>meng<ind>hias</ind></trd>,
    <trd>meng<ind>hias</ind>i</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#39166;&#12427;  <pr>&#12363;&#12374;&#12427;</pr></trd>,
    <trd>&#35013;&#39166;&#12377;&#12427;  <pr>&#12381;&#12358;&#12375;&#12423;&#12367;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">versieren</trd>
  <trdgrp lng="pl">
    <trd>ozdabia&cacute;</trd>,
    <trd>stroi&cacute;</trd>,
    <trd>udekorowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>ornamentar</trd>,
    <trd>decorar</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>orna</trd>,
    <trd>&icirc;mpodobi&tcomma;i</trd>,
    <trd>decora&tcomma;i</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_k;&c_r;&c_a;&c_sh;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">zdobi&tcaron;</trd>
  <trdgrp lng="sv">
    <trd>pryda</trd>,
    <trd>smycka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_i;&c_k;&c_r;&c_a;&c_sh;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_ja;&c_g;&c_a;&c_t;&c_i; &c_p;&c_r;&c_i;&c_k;&c_r;&c_a;&c_s;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20462;&#39166; <pr>xi&umacron;sh&igrave;</pr></trd>,
    <trd>&#20462;&#39280; <pr>xi&umacron;sh&igrave;</pr></trd>,
    <trd>&#39166; <pr>sh&igrave;</pr></trd>,
    <trd>&#39280; <pr>sh&igrave;</pr></trd>,
    <trd>&#35013;&#39280; <pr>zhu&amacron;ngsh&igrave;</pr></trd>,
    <trd>&#35037;&#39166; <pr>zhu&amacron;ngsh&igrave;</pr></trd>,
    <trd>&#28857;&#32512; <pr>di&#462;nzhu&igrave;</pr></trd>,
    <trd>&#40670;&#32180; <pr>di&#462;nzhu&igrave;</pr></trd>,
    <trd>&#35013;&#28857; <pr>zhu&amacron;ngdi&#462;n</pr></trd>,
    <trd>&#35037;&#40670; <pr>zhu&amacron;ngdi&#462;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ornam.0o">
  <kap><tld/>o</kap>
  <!-- ne trovi&gcirc;is ekzemplo de tia uzo:
  <snc mrk="ornam.0o.ado">
    <ref tip="dif" cel="ornam.0ado"><tld/>ado</ref>
    <trdgrp lng="be">
      <trd>&c_u;&c_p;&c_r;&c_y;&c_g;&c_o;&c_zh;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_a;&c_z;&c_d;&c_a;&c_b;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Schm&uuml;cken</trd>,
      <trd>Verzieren</trd>
    </trdgrp>
    <trdgrp lng="zh">
      <trd>&#39166; <pr>sh&igrave;</pr></trd>,
      <trd>&#39280; <pr>sh&igrave;</pr></trd>,
      <trd>&#35013;&#39280;&#29289; <pr>zhu&amacron;ngsh&igrave;w&ugrave;</pr></trd>,
      <trd>&#35037;&#39166;&#29289; <pr>zhu&amacron;ngsh&igrave;w&ugrave;</pr></trd>,
      <trd>&#29910; <pr>xi&amacron;ng</pr></trd>,
      <trd>&#34255;&#21697; <pr>c&aacute;ngp&#464;n</pr></trd>
    </trdgrp>
  </snc>
  -->
  <snc mrk="ornam.0o.ajxo">
    <dif>
      Tio, kio <tld/>as; la <ref tip="dif" cel="ornam.0ajxo"><tld/>a&jcirc;o</ref>j de io:
      <ekz>
        jen renkonte al li iras virino en <tld/>o de publikulino
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 7:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        malgranda bufedo kun la ordinara <tld/>o de boteloj kaj glasoj kaj kun solida
        horlo&gcirc;o
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia belega persono estis vera <tld/>o de la festeno
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkfoje la plej bona <tld/>o estas nenia <tld/>o
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>o por maljunuloj estas grizeco
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 20:29</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_z;&c_d;&c_o;&c_b;&c_a;</trd>
    <trdgrp lng="hu">
      <trd>d&iacute;sz</trd>,
      <trd>d&iacute;sz&iacute;tm&eacute;ny</trd>,
      <trd>ornamentum</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Schmuck</trd>,
      <trd>Verzierung</trd>,
      <trd>Zierrat</trd>,
      <trd>Ornament</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>ornament</trd>,
    <trd>ozdoba</trd>,
    <trd>zdoben&iacute;</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>hias</ind>an</trd>,
    <trd>ornamen</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#39166;&#12426;&#12388;&#12369;  <pr>&#12363;&#12374;&#12426;&#12388;&#12369;</pr></trd>,
    <trd>&#39166;&#12426;  <pr>&#12363;&#12374;&#12426;</pr></trd>,
    <trd>&#35013;&#39166;  <pr>&#12381;&#12358;&#12375;&#12423;&#12367;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ornament</trd>,
    <trd>ozdoba</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>ornament</trd>,
    <trd>podoab&abreve;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ozdoba</trd>,
    <trd>v&yacute;zdoba</trd>
  </trdgrp>
</drv>

<drv mrk="ornam.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Provizado de loko a&ubreve; objekto per <tld/>a&jcirc;oj:
      <ekz>
        &ccirc;iuj konataj fremdlandoj &scirc;ajne kontribuis el siaj plej bonaj trezoroj al la
        konstruado kaj <tld/>ado de lo&gcirc;ejo, inda je potenca reganto
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[nenio]</klr> montrus &ccirc;e la a&ubreve;toro la intencon de arta <tld/>ado, &gcirc;i estis
        verkita en maniero simpla, sana kaj agrabla, multe pli bela, ol <tld/>ita
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">d&iacute;sz&iacute;tm&eacute;s</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_p;&c_r;&c_y;&c_g;&c_o;&c_zh;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_a;&c_z;&c_d;&c_a;&c_b;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="cs">zdoben&iacute;</trd>
  <trdgrp lng="de">
    <trd>Schm&uuml;cken</trd>,
    <trd>Verzieren</trd>
  </trdgrp>
  <trd lng="el">&delta;&jota;&alfa;&kappa;&omikron_ton;&sigma;&my;&eta;&sigma;&eta;</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;coration<klr> (action)</klr></trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>hias</ind>an</trd>,
    <trd>ornamen</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#39166;&#12426;&#12388;&#12369;  <pr>&#12363;&#12374;&#12426;&#12388;&#12369;</pr></trd>,
    <trd>&#35013;&#39166;  <pr>&#12381;&#12358;&#12375;&#12423;&#12367;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ornamentyka</trd>,
    <trd>zdobnictwo</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>ornamenta&ccedil;&atilde;o</trd>,
    <trd>decora&ccedil;&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>ornamentare</trd>,
    <trd>arta decorativ&abreve;</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_k;&c_r;&c_a;&c_sh;&c_je;&c_n;&c_i;&c_je;<klr> (&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">zdobenie</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_i;&c_k;&c_r;&c_a;&c_sh;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_k;&c_r;&c_a;&c_sh;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_k;&c_r;&c_a;&c_sh;&c_je;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#39166; <pr>sh&igrave;</pr></trd>,
    <trd>&#39280; <pr>sh&igrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ornam.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Plibeliga&jcirc;o; unuopa objekto a&ubreve; detalo, kiu servas por <tld/>i:
      <ekz>
        longa tuniko kun oraj <tld/>a&jcirc;oj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili ekfunebris, kaj neniu metis sur sin siajn <tld/>a&jcirc;ojn
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 33:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        troigo de &ccirc;iaspecaj retorikaj figuroj kaj aliaj <tld/>a&jcirc;oj
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>.
