<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ornam.xml,v 1.24 2022/03/24 15:32:04 revo Exp $">
<kap>
  <ofc>*</ofc> <rad>ornam</rad>/i
</kap>
<drv mrk="ornam.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ornam.0i.iu">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Aldoni al tuto plibeligajn detalojn:
      <ekz>
        <tld/>i tablon per
        floroj, <tld/>i &ccirc;ambron per pentra&jcirc;oj;
      </ekz>
      <ekz>
        e&ccirc; &scirc;tipo estos bela, se vi &gcirc;in <tld/>os
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        flag<tld/>i domon, &scirc;ipon.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="dekora.0i">dekoracii</ref>
    </refgrp>
  </snc>
  <snc mrk="ornam.0i.io">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Servi por <tld/>i <sncref ref="ornam.0i.iu"/>:
      <ekz>
        kiun vesto <tld/>as, tiun homoj ekamas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_p;&c_r;&c_y;&c_g;&c_o;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_z;&c_d;&c_a;&c_b;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>b&yacute;t zdobn&yacute;</trd>,
    <trd>vyzdobit</trd>,
    <trd>zdobit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schm&uuml;cken</trd>,
    <trd>verzieren</trd>
  </trdgrp>
  <trd lng="el">&delta;&jota;&alfa;&sigma;&kappa;&omikron;&my;&omega_ton;</trd>
  <trd lng="en">adorn</trd>
  <trd lng="es">adornar</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;corer</trd>,
    <trd>enjoliver</trd>,
    <trd>orner</trd>,
    <trd>ornementer</trd>,
    <trd>parer</trd>
  </trdgrp>
  <trd lng="hu">d&iacute;sz&iacute;t</trd>
  <trdgrp lng="id">
    <trd>meng<ind>hias</ind></trd>,
    <trd>meng<ind>hias</ind>i</trd>
  </trdgrp>
  <trd lng="nl">versieren</trd>
  <trdgrp lng="pl">
    <trd>ozdabia&cacute;</trd>,
    <trd>stroi&cacute;</trd>,
    <trd>udekorowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>ornamentar</trd>,
    <trd>decorar</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>orna</trd>,
    <trd>&icirc;mpodobi&#x21B;i</trd>,
    <trd>decora&#x21B;i</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_k;&c_r;&c_a;&c_sh;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">zdobi&tcaron;</trd>
  <trdgrp lng="sv">
    <trd>pryda</trd>,
    <trd>smycka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x440;&#x438;&#x43A;&#x440;&#x430;&#x448;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>,
    <trd>&#x43D;&#x430;&#x434;&#x44F;&#x433;&#x430;&#x442;&#x438; &#x43F;&#x440;&#x438;&#x43A;&#x440;&#x430;&#x441;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="ornam.0o">
  <kap><tld/>o</kap>
  <snc mrk="ornam.0o.ado">
    <ref tip="dif" cel="ornam.0ado">ornamado</ref>
    <trdgrp lng="be">
      <trd>&c_u;&c_p;&c_r;&c_y;&c_g;&c_o;&c_zh;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_a;&c_z;&c_d;&c_a;&c_b;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Schm&uuml;cken</trd>,
      <trd>Verzieren</trd>
    </trdgrp>
  </snc>
  <snc mrk="ornam.0o.ajxo">
    <ref tip="dif" cel="ornam.0ajxo">ornama&jcirc;o</ref>
    <trd lng="be">&c_a;&c_z;&c_d;&c_o;&c_b;&c_a;</trd>
    <trdgrp lng="hu">
      <trd>d&iacute;sz</trd>,
      <trd>d&iacute;sz&iacute;tm&eacute;ny</trd>,
      <trd>ornamentum</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Schmuck</trd>,
      <trd>Verzierung</trd>,
      <trd>Zierrat</trd>,
      <trd>Ornament</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>ornament</trd>,
    <trd>ozdoba</trd>,
    <trd>zdoben&iacute;</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>hias</ind>an</trd>,
    <trd>ornamen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ornament</trd>,
    <trd>ozdoba</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>ornament</trd>,
    <trd>podoab&abreve;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ozdoba</trd>,
    <trd>v&yacute;zdoba</trd>
  </trdgrp>
</drv>

<drv mrk="ornam.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Provizado de loko a&ubreve; objekto per <tld/>a&jcirc;oj.
    </dif>
    <trd lng="hu">d&iacute;sz&iacute;tm&eacute;s</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_p;&c_r;&c_y;&c_g;&c_o;&c_zh;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_a;&c_z;&c_d;&c_a;&c_b;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="cs">zdoben&iacute;</trd>
  <trdgrp lng="de">
    <trd>Schm&uuml;cken</trd>,
    <trd>Verzieren</trd>
  </trdgrp>
  <trd lng="el">&delta;&jota;&alfa;&kappa;&omikron_ton;&sigma;&my;&eta;&sigma;&eta;</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;coration<klr> (action)</klr></trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>hias</ind>an</trd>,
    <trd>ornamen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ornamentyka</trd>,
    <trd>zdobnictwo</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>ornamenta&ccedil;&atilde;o</trd>,
    <trd>decora&ccedil;&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>ornamentare</trd>,
    <trd>arta decorativ&abreve;</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_k;&c_r;&c_a;&c_sh;&c_je;&c_n;&c_i;&c_je;<klr> (&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">zdobenie</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x440;&#x438;&#x43A;&#x440;&#x430;&#x448;&#x430;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43F;&#x440;&#x438;&#x43A;&#x440;&#x430;&#x448;&#x443;&#x432;&#x430;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43F;&#x440;&#x438;&#x43A;&#x440;&#x430;&#x448;&#x435;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="ornam.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Detala plibeliga&jcirc;o:
      <ekz>
        arkitekturaj <tld/>oj;
      </ekz>
      <ekz>
        tiu monumento estas la plej bela <tld/>o de la urbo;
      </ekz>
      <ekz>
        jen renkonte al li iras virino en <tld/>o de
        publikulino<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>o por maljunuloj estas grizeco<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="dekora.0o">dekoracio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_z;&c_d;&c_o;&c_b;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Schmuck</trd>,
    <trd>Verzierung</trd>,
    <trd>Zierrat</trd>,
    <trd>Ornament</trd>
  </trdgrp>
  <trd lng="el">&delta;&jota;&alfa_ton;&sigma;&kappa;&omikron;&my;&omikron;</trd>
  <trd lng="es">adorno</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;coration<klr> (ornement)</klr></trd>,
    <trd>ornement</trd>,
    <trd>parure</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>d&iacute;sz</trd>,
    <trd>d&iacute;sz&iacute;tm&eacute;ny</trd>,
    <trd>ornamentum</trd>,
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>hias</ind>an</trd>,
    <trd>ornamen</trd>
  </trdgrp>
  <trd lng="nl">versiering</trd>
  <trd lng="pl">ornament</trd>
  <trdgrp lng="pt">
    <trd>ornamento</trd>,
    <trd>enfeite</trd>
  </trdgrp>
  <trd lng="ro">ornament</trd>
  <trd lng="ru">&c_u;&c_k;&c_r;&c_a;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x440;&#x438;&#x43A;&#x440;&#x430;&#x441;&#x430;</trd>,
    <trd>&#x43E;&#x440;&#x43D;&#x430;&#x43C;&#x435;&#x43D;&#x442;</trd>,
    <trd>&#x443;&#x437;&#x43E;&#x440;</trd>
  </trdgrp>
</drv>

<drv mrk="ornam.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Tiu, kiu fabrikas a&ubreve; metas <tld/>a&jcirc;ojn
      por plibeligi ekzemple hejmon.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_k;&c_a;&c_r;&c_a;&c_t;&c_a;&c_r;</trd>
  <trd lng="cs">dekorat&eacute;r</trd>
  <trdgrp lng="fr">
    <trd>architecte d'int&eacute;rieur</trd>,
    <trd>d&eacute;corateur</trd>,
    <trd>ornemaniste</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>d&iacute;sz&iacute;t&odblac;m&udblac;v&eacute;sz</trd>,
    <trd>dekorat&odblac;r</trd>,
    <trd>ornamentalista</trd>
  </trdgrp>
  <trd lng="id">ornamentalis</trd>
  <trd lng="nl">decorateur</trd>
  <trd lng="pl">sztukator</trd>
  <trd lng="pt">ornamentista</trd>
  <trdgrp lng="ro">
    <trd>tencuitor</trd>,
    <trd>stucator</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_je;&c_k;&c_o;&c_r;&c_a;&c_t;&c_o;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>okr&aacute;&scaron;&lcaron;ovac&iacute; technik</trd>,
    <trd>ozdobovac&iacute; umelec</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x430;&#x439;&#x441;&#x442;&#x435;&#x440; &#x437; &#x43E;&#x440;&#x43D;&#x430;&#x43C;&#x435;&#x43D;&#x442;&#x443;</trd>,
    <trd>&#x434;&#x435;&#x43A;&#x43E;&#x440;&#x430;&#x442;&#x43E;&#x440;</trd>
  </trdgrp>
</drv>

<drv mrk="ornam.melodi0o">
  <kap>melodi<tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Kromtonoj, per kiuj muzikanto ofte improvize ornamas melodion,
      tamen ne rompante ties ritmon.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>ornement musical</trd>
  </trdgrp>
  <trd lng="hu">zenei d&iacute;sz&iacute;tm&eacute;ny</trd>
  <trd lng="id">ornamen<klr tip="amb"> (musik)</klr></trd>
  <trd lng="pl">ozdoba muzyczna</trd>
  <trdgrp lng="pt">
    <trd>ornato</trd>,
    <trd>ornamento</trd>
  </trdgrp>
  <trd lng="ro">ornament muzical</trd>
  <trd lng="ru">&c_m;&c_je;&c_l;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
