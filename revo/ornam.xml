<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc> <rad>ornam</rad>/i</kap>
<drv mrk="ornam.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Aldoni al tuto plibeligajn detalojn:
      <ekz>
        <tld/>i tablon per
        floroj, <tld/>i &ccirc;ambron per pentra&jcirc;oj;
      </ekz>
      <ekz>e&ccirc; &scirc;tipo estos bela, se vi &gcirc;in <tld/>os
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        flag<tld/>i domon, &scirc;ipon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>schm&uuml;cken</trd>,
    <trd>verzieren</trd>
  </trdgrp>
  <trd lng="en">adorn</trd>
  <trd lng="nl">versieren</trd>
  <trdgrp lng="sv">
    <trd>pryda</trd>,
    <trd>smycka</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_k;&c_r;&c_a;&c_sh;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="ornam.0ajxo">
  <kap><tld/>(a&jcirc;)o</kap>
  <snc>
    <dif>
      Detala plibeliga&jcirc;o:
      <ekz>
        arkitekturaj <tld/>oj;
      </ekz>
      <ekz>
        tiu monumento estas la plej bela <tld/>o de la urbo;
      </ekz>
      <ekz>
        jen renkonte al li iras virino en <tld/>o de
        publikulino<fnt>Z</fnt>;
      </ekz>
      (f)
      <ekz>
        <tld/>o por maljunuloj estas grizeco<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Schmuck</trd>,
    <trd>Verzierung</trd>,
    <trd>Zierrat</trd>,
    <trd>Ornament</trd>
  </trdgrp>
  <trd lng="nl">versiering</trd>
  <trd lng="ru">&c_u;&c_k;&c_r;&c_a;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="ornam.0ado0arto">
  <kap><tld/>ado, <tld/>arto</kap>
  <snc>
    <dif>
      Arto <tld/>i.
    </dif>
    <refgrp tip="sin">
      <ref cel="dekora.0o">dekoracio</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">decoratiekunst</trd>
</drv>

<drv mrk="ornam.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Tiu, kiu fabrikas <tld/>a&jcirc;ojn por plibeligi hejmon.
    </dif>
  </snc>
  <trd lng="nl">decorateur</trd>
</drv>
</art>
