<art mrk="$Id: sinjor.xml,v 1.72 2021/06/18 04:35:57 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sinjor</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sinjor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <mlg>s-ro</mlg>
  <snc mrk="sinjor.0o.ulo">
    <dif>
      Titolo de &gcirc;entileco por viroj:
      <ekz>
        mi deziras al vi bonan tagon, <tld/>o
        <fnt><bib>F</bib>, <lok>&FE; 10</lok></fnt>!
      </ekz>
      <ekz>
        <tld/>o Petro kaj lia edzino tre amas miajn infanojn
        <fnt><bib>F</bib>, <lok>&FE; 18</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="fr">monsieur</trd>
    <trd lng="hu">&uacute;r</trd>
    <trd lng="ru">&c_g;&c_o;&c_s;&c_p;&c_o;&c_d;&c_i;&c_n;</trd>
    <trd lng="zh">&#x5148;&#x751F; [<ind>xi&#x101;nsheng</ind>]</trd>
  </snc>
  <snc mrk="sinjor.0o.mastro">
    <dif>
      Supera <ref tip="super" cel="cxef.0o">&ccirc;efo</ref>,
      <ref tip="sin" cel="mastr.0o">mastro</ref>:
      <ekz>
        ne kalumniu sklavon anta&ubreve; lia <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 30:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>li estas sia propra <tld/>o</ind>
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
        <trd lng="de">er ist sein eigener <ind>Herr</ind></trd>
        <trd lng="hu">a maga ura</trd>
      </ekz>
      <ekz>
        estu <tld/>o de via vorto
        (<ref tip="vid" cel="promes.0o">promeso</ref>)
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la plej granda <tld/>o estas tiu, kiu bezonas la plej
        malmultan servadon;
      </ekz>
      <ekz>
        ne longe <tld/>a da&ubreve;ras favoro
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="fr">monsieur</trd>
    <trdgrp lng="hu">
      <trd>&uacute;r</trd>,
      <trd>gazda</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_g;&c_o;&c_s;&c_p;&c_o;&c_d;&c_i;&c_n;</trd>,
      <trd>&c_h;&c_o;&c_z;&c_ja;&c_i;&c_n;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sinjor.0o.nobelo">
    <dif>
      En malnova socio, titolo de &gcirc;entileco por nobeloj.
      <ekz>
        al &ccirc;iu <tld/>o estu la honoro
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">seigneur</trd>
    <trd lng="hu">nemes&uacute;r</trd>
    <trd lng="ru">&c_s;&c_je;&c_n;&c_mol;&c_o;&c_r;</trd>
  </snc>
  <snc mrk="sinjor.0o.BIB">
    <uzo tip="fak">BIB</uzo>
    <dif>
      Dio:
      <ekz>
        venos en Sian templon la <tld lit="S"/>o
        <fnt><bib>MT</bib>, <lok>&Mal; 3:1</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="fr">le <ind>Seigneur</ind></trd>
    <trd lng="hu">az <ind>&Uacute;r</ind></trd>
    <trd lng="ru">&c_g;&c_o;&c_s;&c_p;&c_o;&c_d;&c_mol;</trd>
  </snc>
  <trd lng="ca">senyor</trd>
  <trdgrp lng="cs">
    <trd>p&aacute;n</trd>,
    <trd>signor</trd>,
    <trd>signore</trd>
  </trdgrp>
  <trd lng="de">Herr</trd>
  <trdgrp lng="en">
    <trd>Mister</trd>,
    <trd>Mr.</trd>,
    <trd>M.</trd>,
    <trd>Sir</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>se&ntilde;or</trd>,
    <trd>Sr.</trd>
  </trdgrp>
  <trd lng="id">tuan</trd>
  <trd lng="io">siorulo</trd>
  <trdgrp lng="nl">
    <trd>heer</trd>,
    <trd>meneer</trd>
  </trdgrp>
  <trd lng="pl">pan</trd>
  <trd lng="pt">senhor</trd>
  <trd lng="sk">p&aacute;n</trd>
  <trdgrp lng="tr">
    <trd>bay</trd>,
    <trd>bey</trd>
  </trdgrp>
  <trd lng="vo">s&ouml;l</trd>
</drv>

<drv mrk="sinjor.0ido">
  <kap><tld/>ido</kap>
  <snc mrk="sinjor.0ido.KOMUNE">
    <dif>
      Titolo de &gcirc;entileco por idoj de mastro:
      <ekz>
        la <tld/>ido jam dol&ccirc;e dormis kaj ni devis &scirc;iri lin el sub lia 
        varma litkovrilo 
        <fnt>
          <aut>Lao She, trad. Wang Chongfang</aut>,
          <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0026/">
          Kamelo &Scirc;jangzi</url></vrk>,
          <lok>1988</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">senyoret</trd>
  <trd lng="cs">pansk&yacute; synek</trd>
  <trd lng="fr">jeune monsieur</trd>
  <trd lng="pt">patr&atilde;ozinho</trd>
  <trd lng="sk">pansk&yacute; syn</trd>
</drv>

<drv mrk="sinjor.0ino">
  <kap><ofc>*</ofc><tld/>ino</kap>
  <mlg>s-ino</mlg>
  <snc mrk="sinjor.0ino.ulo">
    <dif>
      Titolo de &gcirc;entileco por virinoj:
      <ekz>
        apena&ubreve; &scirc;i venis al la fonto, &scirc;i vidis unu <tld/>inon, tre ri&ccirc;e
        vestitan
        <fnt><bib>F</bib>, <lok>&FE; 19a</lok></fnt>;
      </ekz>
      <ekz>
        <ctl>Kredu min, <tld/>ino!</ctl>
        <klr>(romano de Cezaro <nom>Rossetti</nom>)</klr>
        <fnt>
          <aut>C. Rossetti</aut>,
          <vrk>Kredu min, sinjorino!</vrk>,
          <lok>1950</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ino Rozo estas nia amikino
        <fnt><vrk>"Sinjorino Rozo estas nia amikino."</vrk><lok>
        <url ref="http://www.duolingo.com/comment/11886248">Duolingo</url></lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="frauxl.0ino.RIM">fra&ubreve;lino, Rim.</ref>
  </snc>
  <snc mrk="sinjor.0ino.RELIGIO">
    <dif>
      Diino:
      <ekz>
        Nia <tld lit="S"/>ino <nom>A&scirc;tarot</nom>.
      </ekz>
    </dif>
  </snc>
  <trd lng="af">lady</trd>
  <trd lng="am">&#x12A5;&#x1218;&#x1264;&#x1275;</trd>
  <trd lng="ar">&#x633;&#x64A;&#x62F;&#x629;</trd>
  <trd lng="bn">&#x9AD;&#x9A6;&#x9CD;&#x9B0;&#x9AE;&#x9B9;&#x9BF;&#x9B2;&#x9BE;</trd>
  <trd lng="ca">senyora</trd>
  <trd lng="co">signura</trd>
  <trd lng="cs">signora <klr>(si&ncaron;ora)</klr></trd>
  <trd lng="da">dame</trd>
  <trd lng="de">Frau <klr>(Titel)</klr></trd>
  <trdgrp lng="en">
    <trd>Ms.</trd>,
    <trd>Mrs.</trd>,
    <trd>Miss</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>se&ntilde;ora</trd>,
    <trd>Sra.</trd>
  </trdgrp>
  <trd lng="et">lady</trd>
  <trd lng="eu">andre</trd>
  <trdgrp lng="fr">
    <trd>dame</trd>,
    <trd>madame</trd>
  </trdgrp>
  <trd lng="fy">dame</trd>
  <trd lng="gl">se&#xF1;ora</trd>
  <trd lng="gu">&#xAAE;&#xAB9;&#xABF;&#xAB2;&#xABE;</trd>
  <trd lng="hi">&#x92E;&#x939;&#x93F;&#x932;&#x93E;</trd>
  <trd lng="hr">dama</trd>
  <trdgrp lng="hu">
  <trd>&uacute;rn&odblac;</trd>
    <trd>asszony</trd>,
    <trd>h&ouml;lgy</trd>
  </trdgrp>
  <trd lng="hy">&#x57F;&#x56B;&#x56F;&#x56B;&#x576;</trd>
  <trd lng="id">nyonya</trd>
  <trd lng="io">siorino</trd>
  <trd lng="is">konan</trd>
  <trd lng="ja">&#x5973;&#x6027;</trd>
  <trd lng="ka">&#x10E5;&#x10D0;&#x10DA;&#x10D1;&#x10D0;&#x10E2;&#x10DD;&#x10DC;&#x10E1;</trd>
  <trd lng="km">&#x179F;&#x17D2;&#x178F;&#x17D2;&#x179A;&#x17B8;</trd>
  <trd lng="ko">&#xC544;&#xAC00;&#xC528;</trd>
  <trd lng="lo">&#xEC0;&#xE9B;&#xEB1;&#xE99;</trd>
  <trd lng="ml">&#xD2F;&#xD41;&#xD35;&#xD24;&#xD3F;&#xD2F;&#xD41;&#xD1F;&#xD46;</trd>
  <trd lng="mr">&#x92E;&#x939;&#x93F;&#x932;&#x93E;</trd>
  <trd lng="my">&#x1021;&#x1019;&#x103B;&#x102D;&#x102F;&#x1038;&#x101E;&#x1019;&#x102E;&#x1038;</trd>
  <trd lng="ne">&#x92E;&#x939;&#x93F;&#x932;&#x93E;</trd>
  <trd lng="nl">mevrouw</trd>
  <trd lng="ny">dona</trd>
  <trd lng="pa">&#xA14;&#xA30;&#xA24;</trd>
  <trd lng="ps">&#x627;&#x628;&#x6CD;</trd>
  <trd lng="pt">senhora</trd>
  <trd lng="pl">pani</trd>
  <trd lng="ru">&c_g;&c_o;&c_s;&c_p;&c_o;&c_zh;&c_a;</trd>
  <trd lng="sd">&#x644;&#x6A9;&#x64A;&#x627;</trd>
  <trd lng="si">&#xD9A;&#xDCF;&#xDB1;&#xDCA;&#xDAD;&#xDCF;&#xDC0;</trd>
  <trd lng="sk">signora <klr>(se&ncaron;ora)</klr></trd>
  <trd lng="sq">lady</trd>
  <trd lng="ta">&#xBAA;&#xBC6;&#xBA3;&#xBCD;</trd>
