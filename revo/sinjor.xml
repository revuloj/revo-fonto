<drv mrk="sinjor.ge0oj">
  <kap>
    ge<tld/>oj
  </kap>
  <snc>
    <dif>
      <tld lit="S"/>o kaj <tld/>ino (a&ubreve; <tld/>oj kaj <tld/>inoj):
      <ekz>
        la &ccirc;ambro de la ge<tld/>oj
        <fnt><bib>Fab1</bib> <lok>Fluganta kofro</lok></fnt>;
      </ekz><ekz>
        ge<tld/>oj <nom>Perkowski</nom> invitis lin al la ekskurso
        <fnt><bib>IK</bib></fnt>;
      </ekz><ekz>
        kiel la ge<tld/>oj, tiel anka&ubreve; la servistoj, e&ccirc; la amikoj de la domo, estis
        feli&ccirc;aj
        <fnt><bib>Fab3</bib> <lok>Lasta perlo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_a;&c_d;&c_a;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_o;&c_v;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Herrschaften</trd>,
    <trd>Damen und Herren</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x592B;&#x59BB;  <pr>&#12405;&#12373;&#12356;</pr></trd>,
    <trd>&#x7D33;&#x58EB;&#x6DD1;&#22899;  <pr>&#12375;&#12435;&#12375;&#12375;&#12421;&#x304F;&#12376;&#12423;</pr></trd>
  </trdgrp>
  <trd lng="pl">pa&nacute;stwo  <klr>(panie i panowie)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_s;&c_p;&c_o;&c_d;&c_a;<klr> (&c_o;&c_b;&c_o;&c_je;&c_g;&c_o; &c_p;&c_o;&c_l;&c_a;)</klr></trd>,
    <trd>&c_d;&c_a;&c_m;&c_y; &c_i; &c_g;&c_o;&c_s;&c_p;&c_o;&c_d;&c_a;</trd>
  </trdgrp>
  <trd lng="uk">&c_p;&c_a;&c_n;&c_o;&c_v;&c_je; &c_ib; &c_p;&c_a;&c_n;&c_ib;</trd>
  <trdgrp lng="zh">
    <trd>&#22899;&#x751F;&#x4EEC;&#20808;&#x751F;&#x4EEC; <pr>n&#x1DA;sh&emacron;ngmenxi&amacron;nsh&emacron;ngmen</pr></trd>,
    <trd>&#22899;&#x751F;&#20497;&#20808;&#x751F;&#20497; <pr>n&#x1DA;sh&emacron;ngmenxi&amacron;nsh&emacron;ngmen</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sinjor.0umi">
  <kap><tld/>umi</kap>
  <snc mrk="sinjor.0umi.MOR">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Senrajte afekti sintenon de <tld/>o, de superulo:
      <ekz>
        la Brita Imperio forsendis milionojn, precipe
        senta&ubreve;gulojn, al siaj kolonioj, por tie <tld/>umi
        super la enlo&gcirc;antoj
        <fnt>
          <aut>K. Miner</aut>,
          <vrk><url ref="http://www.sunflower.com/~miner/TAGLIBRO_package/Oktobro_2005.html">
          Reta taglibro</url></vrk>,
          <lok>2005-10-15</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_a;&c_s;&c_a;&c_b;&c_l;&c_ib;&c_v;&c_a; &c_b;&c_ja;&c_s;&c_p;&c_r;&c_a;&c_w;&c_n;&c_a;)</klr></trd>
    <trdgrp lng="de">
      <trd>herrschen</trd>,
      <trd>sich <ind>aufspielen</ind></trd>,
      <trd>sich <ind>gerieren</ind></trd>
    </trdgrp>
    <trd lng="fr">faire l'important</trd>
    <trd lng="pl">bezprawnie  <ind>panowa&cacute;</ind></trd>
    <trd lng="pt">bancar o poderoso</trd>
  </snc>
  <snc mrk="sinjor.0umi.alparole">
    <gra><vspec>tr</vspec></gra>
    <dif>
      Alparoli per &leftquot;<tld/>o&rightquot;:
      <ekz>
        &mdash; Estimata Sinjoro <klr>[&#8230;]</klr>
        &mdash; Mi &scirc;atus se vi ne <tld/>umus min
        <fnt>
          <aut>S. Johansson</aut>,
          <vrk><url
          ref="https://groups.yahoo.com/neo/groups/per-esperanto-literaturo/conversations/topics/88">
