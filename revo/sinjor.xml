<drv mrk="sinjor.ge0oj">
  <kap>
    ge<tld/>oj
  </kap>
  <snc>
    <dif>
      <tld lit="S"/>o kaj <tld/>ino (a&ubreve; <tld/>oj kaj <tld/>inoj):
      <ekz>
        la &ccirc;ambro de la ge<tld/>oj
        <fnt><bib>Fab1</bib> <lok>Fluganta kofro</lok></fnt>;
      </ekz><ekz>
        ge<tld/>oj <nom>Perkowski</nom> invitis lin al la ekskurso
        <fnt><bib>IK</bib></fnt>;
      </ekz><ekz>
        kiel la ge<tld/>oj, tiel anka&ubreve; la servistoj, e&ccirc; la amikoj de la domo, estis
        feli&ccirc;aj
        <fnt><bib>Fab3</bib> <lok>Lasta perlo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_a;&c_d;&c_a;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_o;&c_v;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Herrschaften</trd>,
    <trd>Damen und Herren</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#22827;&#22971;  <pr>&#12405;&#12373;&#12356;</pr></trd>,
    <trd>&#32051;&#22763;&#28113;&#22899;  <pr>&#12375;&#12435;&#12375;&#12375;&#12421;&#12367;&#12376;&#12423;</pr></trd>
  </trdgrp>
  <trd lng="pl">pa&nacute;stwo  <klr>(panie i panowie)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_s;&c_p;&c_o;&c_d;&c_a;<klr> (&c_o;&c_b;&c_o;&c_je;&c_g;&c_o; &c_p;&c_o;&c_l;&c_a;)</klr></trd>,
    <trd>&c_d;&c_a;&c_m;&c_y; &c_i; &c_g;&c_o;&c_s;&c_p;&c_o;&c_d;&c_a;</trd>
  </trdgrp>
  <trd lng="uk">&c_p;&c_a;&c_n;&c_o;&c_v;&c_je; &c_ib; &c_p;&c_a;&c_n;&c_ib;</trd>
  <trdgrp lng="zh">
