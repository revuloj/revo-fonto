  <trd lng="zu">inenekazi</trd>
</drv>

<drv mrk="sinjor.ge0oj">
  <kap>
    ge<tld/>oj
  </kap>
  <snc>
    <dif>
      <tld lit="S"/>o kaj <tld/>ino (a&ubreve; <tld/>oj kaj <tld/>inoj):
      <ekz>
        la &ccirc;ambro de la ge<tld/>oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Fluganta kofro</lok>
        </fnt>;
      </ekz>
      <ekz>
        ge<tld/>oj <nom>Perkowski</nom> invitis lin al la ekskurso
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiel la ge<tld/>oj, tiel anka&ubreve; la servistoj, e&ccirc; la amikoj de la domo, estis
        feli&ccirc;aj
        <fnt>
          <bib>Fab3</bib>
          <lok>Lasta perlo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Herrschaften</trd>,
    <trd>Damen und Herren</trd>
  </trdgrp>
</drv>

<drv mrk="sinjor.0umi">
  <kap><tld/>umi</kap>
  <snc mrk="sinjor.0umi.MOR">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Senrajte afekti sintenon de <tld/>o, de superulo:
      <ekz>
        la Brita Imperio forsendis milionojn, precipe 
        senta&ubreve;gulojn, al siaj kolonioj, por tie <tld/>umi 
        super la enlo&gcirc;antoj
        <fnt>
          <aut>K. Miner</aut>,
          <vrk><url ref="http://www.sunflower.com/~miner/TAGLIBRO_package/Oktobro_2005.html">
          Reta taglibro</url></vrk>,
          <lok>2005-10-15</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">herrschen</trd>
    <trd lng="fr">faire l'important</trd>
    <trd lng="pt">bancar o poderoso</trd>
  </snc>
  <snc mrk="sinjor.0umi.alparole">
    <gra><vspec>tr</vspec></gra>
    <dif>
      Alparoli per &leftquot;<tld/>o&rightquot;:
      <ekz>
        &mdash; Estimata Sinjoro <klr>[&#x2026;]</klr>
        &mdash; Mi &scirc;atus se vi ne <tld/>umus min
        <fnt>
          <aut>S. Johansson</aut>,
          <vrk><url ref="https://groups.yahoo.com/neo/groups/per-esperanto-literaturo/conversations/topics/88">
          sveda verkisto</url></vrk>,
          <lok>Literaturo en kaj per Esperanto, 2001-12-16</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">donner du <ind>monsieur</ind> &agrave; qqun</trd>
    <trd lng="pt">tratar algu&eacute;m por senhor</trd>
  </snc>
  <trd lng="ca">senyorejar</trd>
</drv>

<drv mrk="sinjor.grand0o">
  <kap>grand<tld/>o</kap>
  <snc mrk="sinjor.grand0o.NOB">
    <dif>
      Altranga <ref tip="super" cel="nobel.0o">nobelo</ref>;
      <ref tip="sin" cel="grand.0ulo.nobelo">grandulo</ref>:
      <ekz>
        grand<tld/>a maniereco
        <fnt>
          <bib>Fab1</bib>
          <lok>Fluganta kofro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd><ind>alt </ind>noble</trd>,
    <trd>arist&ograve;crata</trd>,
    <trd>emin&egrave;ncia</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>grand</trd>,
    <trd>magn&aacute;t</trd>,
    <trd>majetn&yacute; &ccaron;lov&ecaron;k</trd>,
    <trd>velmo&zcaron;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>grand seigneur</trd>,
    <trd>grand<klr> (subst., aristocrate)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>f&odblac;&uacute;r</trd>,
    <trd>nagy&uacute;r</trd>
  </trdgrp>
  <trd lng="nl">grand seigneur</trd>
  <trdgrp lng="pt">
    <trd>aristocrata</trd>,
    <trd>magnata</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_n;&c_d;</trd>
  <trd lng="sk">ve&lcaron;komo&zcaron;n&yacute; p&aacute;n</trd>
</drv>


