<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sinjor</rad>/o
</kap>
<drv mrk="sinjor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <mlg>s-ro</mlg>
  <snc mrk="sinjor.0o.ulo">
    <dif>
      Titolo de &gcirc;entileco por la viroj.
    </dif>
    <trdgrp lng="fr">
      <trd>monsieur</trd>
    </trdgrp>
    <trd lng="ru">&c_g;&c_o;&c_s;&c_p;&c_o;&c_d;&c_i;&c_n;</trd>
    <trd lng="zh">&#20808;&#29983;<klr> [xi&#257;nsheng]</klr></trd>
  </snc>
  <snc mrk="sinjor.0o.mastro">
    <dif>
      Supera &ccirc;efo, mastro:
      <ekz>
        ne kalumniu sklavon anta&ubreve; lia <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li estas sia propra <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        estu <tld/>o de via vorto
        (<ref tip="vid" cel="promes.0o">promeso</ref>)
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la plej granda <tld/>o estas tiu, kiu bezonas la plej
        malmultan servadon;
      </ekz>
      <ekz>
        ne longe <tld/>a da&ubreve;ras favoro
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>monsieur</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_g;&c_o;&c_s;&c_p;&c_o;&c_d;&c_i;&c_n;</trd>,
      <trd>&c_h;&c_o;&c_z;&c_ja;&c_i;&c_n;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sinjor.0o.nobelo">
    <dif>
      <ref tip="super" cel="sinjor.grand0o">grandsinjoro</ref>
      <ekz>
        al &ccirc;iu <tld/>o estu la honoro<fnt>Z</fnt>;
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>seigneur</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_je;&c_n;&c_mol;&c_o;&c_r;</trd>
  </snc>
  <snc mrk="sinjor.0o.dio">
    <dif>
      Dio:
      <ekz>
        la templo de la <tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>le <ind>seigneur</ind></trd>
    </trdgrp>
    <trd lng="ru">&c_g;&c_o;&c_s;&c_p;&c_o;&c_d;&c_mol;</trd>
  </snc>
  <trd lng="cs">p&aacute;n</trd>
  <trd lng="de">Herr</trd>
  <trdgrp lng="en">
    <trd>Mister</trd>,
    <trd>Mr.</trd>,
    <trd>M.</trd>,
    <trd>Sir</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>heer</trd>,
    <trd>meneer</trd>
  </trdgrp>
  <trd lng="pl">pan</trd>
  <trdgrp lng="tr">
    <trd>bay</trd>,
    <trd>bey</trd>
  </trdgrp>
  <trd lng="vo">s&ouml;l</trd>
</drv>

<drv mrk="sinjor.0ino">
  <kap><tld/>ino</kap>
  <mlg>s-ino</mlg>
  <snc>
    <dif>
      &Gcirc;entila titolo uzata por edzini&gcirc;intaj virinoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="frauxl.0ino">fra&ubreve;lino</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Frau</trd>
  <trdgrp lng="fr">
    <trd>dame</trd>,
    <trd>madame</trd>
  </trdgrp>
  <trd lng="nl">mevrouw</trd>
  <trd lng="pl">pani</trd>
  <trdgrp lng="tr">
    <trd>bayan</trd>,
    <trd>han&inodot;m</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_o;&c_s;&c_p;&c_o;&c_zh;&c_a;</trd>
</drv>

<drv mrk="sinjor.grand0o">
  <kap>grand<tld/>o</kap>
  <snc>
    <dif>
      Altranga <ref tip="super" cel="nobel.0o">nobelo</ref>.
    </dif>
    <refgrp tip="sin">
      <ref cel="grand.0ulo.nobelo">grandulo</ref>,
      <ref cel="sinjor.0o.nobelo">sinjoro</ref>
    </refgrp>
    <ekz>
      grand<tld/>aj manieroj<fnt>Z</fnt>.
    </ekz>
  </snc>
  <trdgrp lng="fr">
    <trd>grand<klr> (aristocrate)</klr></trd>
  </trdgrp>
  <trd lng="nl">grand seigneur</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_n;&c_d;</trd>
</drv>
</art>
