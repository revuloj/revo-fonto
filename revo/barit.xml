<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: barit.xml,v 1.20 2023/10/14 08:39:48 revo Exp $">
<kap>
  <rad>barit</rad>/o
</kap>

<drv mrk="barit.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      <frm>BaSO<sub>4</sub></frm>, baria sulfato, peza spato:
      <ekz>
        en <nom>Ningxia</nom> <klr>[&#x2026;]</klr> estas anka&ubreve; abundo da nemetalaj mina&jcirc;oj, kiaj karbo,
        gipso, nafto, natura gaso, kaolino, siliko kaj <tld/>o
        <fnt>
          <vrk><url ref="http://esperanto.china.org.cn/world/shi-xibu/ningxia-shj.htm">Ningxia,
          Geografio kaj natura stato</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la sulfata a&ubreve; ka&ubreve;stika <tld/>o estas <tld/>a akvo 
        (solva&jcirc;o de <frm><k>Ba</k>[<k>SO</k><sub>4</sub>]</frm> en akvo)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=577212">Barito</url></lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="be">&c_b;&c_a;&c_r;&c_y;&c_t;</trd>
  <trdgrp lng="de">
    <trd>Baryt</trd>,
    <trd>Bariummsulfat</trd>,
    <trd>Schwerspat</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>barite</trd>
  </trdgrp>
  <trd lng="fr">baryte</trd>
  <trd lng="nl">bariet</trd>
  <trdgrp lng="nl">
    <trd>bariet</trd>
  </trdgrp>
  <trd lng="pl">baryt</trd>
  <trd lng="ru">&c_b;&c_a;&c_r;&c_i;&c_t;</trd>
</snc>
  <trd lng="ja">&#x91CD;&#x6676;&#x77F3;  <pr>&#x3058;&#x3085;&#x3046;&#x3057;&#x3087;&#x3046;&#x305B;&#x304D;</pr></trd>
  <trd lng="uk">&c_b;&c_a;&c_r;&c_i;&c_t;</trd>
</drv>

<drv mrk="barit.0papero">
<kap><tld/>papero</kap>
<uzo tip="fak">FOT</uzo>
<uzo tip="fak">TIP</uzo>
<snc>
  <dif>
    Papero, &scirc;mirita per barita tavolo kaj uzata por specialaj
    laboroj fotografaj a&ubreve; en presarto.
  </dif>
  <trd lng="de">Barytpapier</trd>
  <trd lng="en">glossy <ind>paper</ind></trd>
  <trdgrp lng="fr">
     <trd>baryte <klr tip="ind">(papier)</klr></trd>,
     <trd>papier <ind>baryt&eacute;</ind></trd></trdgrp>
  <trd lng="hu">baritpap&iacute;r</trd>
  <trd lng="nl">barietpapier</trd>
</snc>
<trd lng="be">&c_g;&c_l;&c_ja;&c_n;&c_c;&c_a;&c_v;&c_a;&c_n;&c_a;&c_ja; <ind>&c_p;&c_a;&c_p;&c_je;&c_r;&c_a;</ind></trd>
  <trdgrp lng="cs">
    <trd>barytov&yacute; pap&iacute;r</trd>,
    <trd>fotografick&yacute; pap&iacute;r <klr>(hist.)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x30D0;&#x30E9;&#x30A4;&#x30BF;&#x7D19;  <pr>&#x30D0;&#x30E9;&#x30A4;&#x30BF;&#x3057;</pr></trd>
  <trd lng="sk">barytov&yacute; papier</trd>
</drv>
</art>
