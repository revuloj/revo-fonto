<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kebeki.xml,v 1.17 2023/12/03 09:58:48 revo Exp $">
<kap>
  <rad>Kebeki</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kebeki.K0o">
  <kap><tld/>o</kap>
  <snc mrk="kebeki.K0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="kebek.K0o.provinco">Kebeko<sncref/></ref>:
      <ekz>
        kadre de la Trakta&jcirc;o de Parizo, Francio donas al Anglio &ccirc;iujn
        siajn amerikajn teritoriojn, kaj Anglio kreas la provincon <tld/>o
        <fnt>
          <aut>Esperanto-Societo Kebekia</aut>,
          <vrk><url ref="http://www.esperanto.qc.ca/html/quebec-historio-eo-u.htm">Informoj
          pri Kebekio</url></vrk>,
          <lok>[vidita en 2012]</lok>
        </fnt>;
      </ekz>
      <ekz>
        en <tld/>o la <tld lit="k"/>anoj etne formas integran nacion, regatan tamen de
        anglo-saksa re&gcirc;imo
        <fnt>
          <aut>Remigiusz Mielcarek</aut>
          <vrk><url ref="&Monato;/1994/A940101.php">Enmigrintoj en ora ka&gcirc;o</url></vrk>
          <bib>Monato</bib>
          <lok>1994/01, p. 15</lok>
        </fnt>;
      </ekz>
      <ekz>
        vel&scirc;ipon, kiu tenis 700 tonelojn, konstruis skoto en 1847 en <tld/>o
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2004/008277.html">Renaski&gcirc;o de vel&scirc;ipo Jeanie Johnson</url></vrk>
          <bib>Monato</bib>
          <lok>2004/05, p. 21</lok>
        </fnt>;
      </ekz>
      <ekz>
        la franclingva kanada provinco <tld/>o provas plian fojon fari&gcirc;i la plej nova
        nord-amerika regno
        <fnt>
          <aut>Lenio Marobin</aut>
          <vrk><url ref="&Monato;/2014/011316.php">Grava elemento de monda nestabileco</url></vrk>
          <bib>Monato</bib>
          <lok>2014/10, p. 25</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_K;&c_v;&c_e;&c_b;&c_e;&c_k;</trd>
  <trd lng="bg">&c_K;&c_v;&c_je;&c_b;&c_je;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>Quebec</trd>,
    <trd>Quebek</trd>
  </trdgrp>
  <trd lng="de">Qu&eacute;bec</trd>
  <trd lng="en">Quebec</trd>
  <trd lng="es">Quebec</trd>
  <trd lng="fr">Qu&eacute;bec<klr> (province)</klr></trd>
  <trd lng="ja">&#x30B1;&#x30D9;&#x30C3;&#x30AF;&#x5DDE;  <pr>&#x30B1;&#x30D9;&#x30C3;&#x30AF;&#x3057;&#x3085;&#x3046;</pr></trd>
  <trd lng="nl">Qu&eacute;bec</trd>
  <trd lng="pl">Quebec</trd>
  <trdgrp lng="sk">
    <trd>Quebec</trd>,
    <trd>Quebek</trd>
  </trdgrp>
  <trd lng="ro">Qu&eacute;bec&gt;</trd>
  <trd lng="uk">&#x41A;&c_v;&c_je;&c_b;&c_je;&#x43A;</trd>
  <trd lng="zh">&#x9B41;&#x5317;&#x514B; <pr>Ku&#xED;b&#x11B;ik&#xE8;</pr></trd>
</drv>
</art>

