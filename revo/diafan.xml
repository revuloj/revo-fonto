<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: diafan.xml,v 1.30 2023/11/04 14:29:01 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>diafan</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="diafan.0a">
  <kap><tld/>a</kap>
  <snc mrk="diafan.0a.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      Trairebla per lumradioj, sed ne <ref cel="vid.tra0ebla">travidebla</ref>:
      <ekz>
        paperaj lanternoj radiis kiel grandaj <tld/>aj tulipoj
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>;
      </ekz>
      <ekz>
        belegaj floroj staris en brilantaj <tld/>aj vazoj
        <fnt>
          <bib>Fab3</bib>
          <lok>Ekzistas diferenco</lok>
        </fnt>;
      </ekz>
      <ekz>
        meze de la floro sidis malgranda viro, tiel blanka kaj <tld/>a, kvaza&ubreve; li estus
        el vitro
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ru&gcirc;a <tld/>a inko
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a kvarco
        <fnt>
          <bib>Kandid</bib>
        </fnt>;
      </ekz>
      <ekz>
        diafana muslino
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        sub la <tld/>a &scirc;tofo oni vidis &scirc;iajn virgajn brustojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        tra la <tld/>a blankeco de &scirc;ia viza&gcirc;o en diversaj lokoj montri&gcirc;is flavaj strioj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        tra la <tld/>a ha&ubreve;to oni povis preska&ubreve; vidi liajn ostojn
        <fnt>
          <bib>QuV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="klar.0a">klara</ref>,
      <ref cel="lakt.0eca.simila">lakteca<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_z;&c_r;&c_y;&c_s;&c_t;&c_y;</trd>
  <trd lng="ca">transl&uacute;cid</trd>
  <trd lng="cs">pr&uring;svitn&yacute; <klr>(ne pr&uring;hledn&yacute;)</klr></trd>
  <trdgrp lng="de">
    <trd>durchscheinend</trd>,
    <trd>milchig</trd>,
    <trd>halbtransparent</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>diaphanous</trd>,
    <trd>translucent</trd>,
    <trd>semi-transparent</trd>
  </trdgrp>
  <trd lng="es">transl&uacute;cido</trd>
  <trdgrp lng="fr">
    <trd>diaphane</trd>,
    <trd>translucide</trd>
  </trdgrp>
  <trd lng="he">&shin;&#x5C1;&#x5B8;&qof;&vav;&#x5BC;&fpe;</trd>
  <trd lng="hu">&aacute;ttetsz&odblac;</trd>
  <trd lng="ja">&#x534A;&#x900F;&#x660E;&#x306E;  <pr>&#x306F;&#x3093;&#x3068;&#x3046;&#x3081;&#x3044;&#x306E;</pr></trd>
  <trd lng="nl">doorschijnend</trd>
  <trdgrp lng="pl">
    <trd>prze&sacute;wituj&aogonek;cy</trd>,
    <trd>prze&sacute;wiecaj&aogonek;cy</trd>,
    <trd>p&oacute;&lstroke;prze&zacute;roczysty</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>di&aacute;fano</trd>,
    <trd>transparente</trd>,
    <trd>vaporoso</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_z;&c_r;&c_a;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_v;&c_je;&c_ch;&c_i;&c_v;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">priesvitn&yacute;</trd>
  <trd lng="uk">&#x43F;&#x440;&#x43E;&#x437;&#x43E;&#x440;&#x438;&#x439;</trd>
  <trdgrp lng="zh">
    <trd>&#x900F;&#x5149; <pr>t&#xF2;ugu&#x101;ng</pr></trd>,
    <trd>&#x534A;&#x900F;&#x660E;&#x7684; <pr>b&#xE0;nt&#xF2;um&#xED;ngde</pr></trd>
  </trdgrp>
</drv>

<drv mrk="diafan.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="diafan.mal0a.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      Ne a&ubreve; tre malmulte tralasanta lumon:
      <ekz>
        sur malpurigitaj bordoj de mal<tld/>aj lagoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la mal<tld/>a, ekstera kovra&jcirc;o rompi&gcirc;as pro la 
        ekfrapo, malkovrante vitran cilindron
        <fnt>
          <aut>E. R. Burroughs, trad. K. R. C. Sturmer</aut>,
          <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0029/index.php">
          Princino de Marso</url></vrk>,
          <lok>1938</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la procezo de politika decido fari&gcirc;is samtempe pli mal<tld/>a kaj pli fragmentita
        <fnt>
          <bib>MD14</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="klar.mal0a">malklara</ref>,
      <ref cel="obskur.0a.malklara">obskura</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_p;&c_r;&c_a;&c_z;&c_r;&c_y;&c_s;&c_t;&c_y;</trd>
  <trd lng="cs">nepr&uring;svitn&yacute;</trd>
  <trdgrp lng="de">
    <trd>undurchsichtig</trd>,
    <trd>tr&uuml;b</trd>,
    <trd>opak</trd>
  </trdgrp><trd lng="fr">opaque</trd>
  <trd lng="ja">&#x4E0D;&#x900F;&#x660E;&#x306A;  <pr>&#x3075;&#x3068;&#x3046;&#x3081;&#x3044;&#x306A;</pr></trd>
  <trdgrp lng="nl">
    <trd>ondoorschijnend</trd>,
    <trd>opaak</trd>
  </trdgrp>
  <trd lng="sk">nepriesvitn&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x43D;&#x435;&#x43F;&#x440;&#x43E;&#x437;&#x43E;&#x440;&#x438;&#x439;</trd>,
    <trd>&#x43A;&#x430;&#x43B;&#x430;&#x43C;&#x443;&#x442;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x43C;&#x443;&#x442;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6349;&#x6478;&#x4E0D;&#x5B9A; <pr>zhu&#x14D;m&#xF3;b&#xF9;d&#xEC;ng</pr></trd>,
    <trd>&#x6DF7;&#x6D4A; <pr>h&#xFA;nzhu&#xF3;</pr></trd>,
    <trd>&#x74C0; <pr>ru&#x1CE;n</pr></trd>
  </trdgrp>
</drv>
</art>
