<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>diafan</rad>/a
</kap>
<drv mrk="diafan.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Trairebla per lumradioj, sed ne travidebla:
      <ekz>
        <tld/>a ŝtofo, ondo, vazo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a emajlo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>aj manetoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">празрысты</trd>
  <trd lng="ca">translúcid</trd>
  <trdgrp lng="de">
    <trd>durchscheinend</trd>,
    <trd>milchig</trd>,
    <trd>halbtransparent</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>diaphanous</trd>,
    <trd>translucent</trd>,
    <trd>semi-transparent</trd>
  </trdgrp>
  <trd lng="es">translúcido</trd>
  <trdgrp lng="fr">
    <trd>diaphane</trd>,
    <trd>translucide</trd>
  </trdgrp>
  <trd lng="hu">áttetsző</trd>
  <trdgrp lng="pt">
    <trd>diáfano</trd>,
    <trd>transparente</trd>,
    <trd>vaporoso</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>прозрачный</trd>,
    <trd>просвечивающий</trd>
  </trdgrp>
  <trd lng="sk">priesvitný</trd>
</drv>
</art>
