<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>diafan</rad>/a
</kap>
<drv mrk="diafan.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Trairebla per lumradioj, sed ne travidebla:
      <ekz>
        <tld/>a &scirc;tofo, ondo, vazo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a emajlo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>aj manetoj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>durscheinend</trd>,
    <trd>milchig</trd>,
    <trd>halbtransparent</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>diaphane</trd>,
    <trd>translucide</trd>
  </trdgrp>
  <trd lng="hu">&aacute;ttetsz&odblac;</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_z;&c_r;&c_a;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_v;&c_je;&c_ch;&c_i;&c_v;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">priesvitn&yacute;</trd>
</drv>
</art>
