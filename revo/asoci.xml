<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: asoci.xml,v 1.37 2020/02/13 18:10:25 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>asoci</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="asoci.0o">
  <kap><tld/>o</kap>
  <snc mrk="asoci.0o.societo">
    <dif>
      Societo por komuna celo;
      <ref tip="sin" cel="lig.0o.asocio">ligo<sncref/></ref>.
    </dif>    
    <refgrp tip="vid">
      <ref cel="grup.0o">grupo</ref>,
      <ref cel="kompan.0o.direktantaro">kompanio<sncref/></ref>,
      <ref cel="unu">unui&gcirc;o</ref>
    </refgrp>    
    <trdgrp lng="be">
      <trd>&c_a;&c_s;&c_a;&c_c;&c_y;&c_ja;&c_c;&c_y;&c_ja;</trd>,
      <trd>&c_s;&c_u;&c_p;&c_o;&c_l;&c_k;&c_a;</trd>,
      <trd>&c_g;&c_r;&c_a;&c_m;&c_a;&c_d;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>sz&ouml;vets&eacute;g<klr tip="ind">
      (t&aacute;rsas&aacute;g)</klr></trd>,
      <trd>t&aacute;rsas&aacute;g<klr tip="ind">
      (sz&ouml;vets&eacute;g)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_a;&c_s;&c_s;&c_o;&c_c;&c_i;&c_a;&c_c;&c_i;&c_ja;</trd>,
      <trd>&c_o;&c_b;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
    </trdgrp>
    <trd lng="th">&#3626;&#3617;&#3634;&#3588;&#3617;</trd>
  </snc>
  <snc mrk="asoci.0o.kunligiteco">
    <dif>
      Kun<ref tip="super" cel="lig.0o.asocio">lig</ref>iteco,
      kun<ref tip="vid" cel="rilat.0o.ligo">rilat</ref>eco:
      <ekz>
        <tld/>o de la ideoj.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_s;&c_a;&c_c;&c_y;&c_ja;&c_c;&c_y;&c_ja;</trd>,
      <trd>&c_s;&c_u;&c_v;&c_ja;&c_z;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>asszoci&aacute;ci&oacute;</trd>,
      <trd>t&aacute;rs&iacute;t&aacute;s</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>asociace</trd>,
    <trd>jednota</trd>,
    <trd>sdru&zcaron;en&iacute;</trd>,
    <trd>spolek</trd>,
    <trd>svaz</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gesellschaft</trd>,
    <trd>Verein</trd>
  </trdgrp>
  <trd lng="en">association</trd>
  <trd lng="es">asociaci&oacute;n</trd>
  <trd lng="fr">association</trd>
  <trd lng="it">associazione</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_s;&c_s;&c_o;&c_c;&c_i;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_s;&c_v;&c_ja;&c_z;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>organiz&aacute;cia</trd>,
    <trd>spolok</trd>,
    <trd>zv&auml;z</trd>
  </trdgrp>
</drv>

<drv mrk="asoci.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="asoci.0i.kungrupigi">
    <dif>
      Kungrupigi en <tld/>on.
    </dif>
    <trd
    lng="be">&c_z;&c_g;&c_r;&c_u;&c_p;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="hu">
      <trd>sz&ouml;vets&eacute;gbe t&ouml;m&ouml;r&iacute;t</trd>,
      <trd>t&aacute;rs&iacute;t</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_g;&c_r;&c_u;&c_p;&c_p;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_g;&c_r;&c_u;&c_p;&c_p;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="th">&#3648;&#3586;&#3657;&#3634;&#3619;&#3656;&#3623;&#3617;&#3626;&#3617;&#3634;&#3588;&#3617;</trd>
  </snc>
  <snc mrk="asoci.0i.kunligi">
    <dif>
      Kunligi:
      <ekz>
        Per <tld/>ado de la ideoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_s;&c_a;&c_c;&c_y;&c_ja;&c_c;&c_y;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_l;&c_u;&c_ch;&c_y;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>t&aacute;rs&iacute;t</trd>,
      <trd>asszoci&aacute;l</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_v;&c_ja;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
    <trd lng="th">&#3617;&#3637;&#3626;&#3656;&#3623;&#3609;&#3619;&#3623;&#3617;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>asociovat</trd>,
    <trd>sdru&zcaron;it</trd>,
    <trd>sdru&zcaron;ovat</trd>,
    <trd>spojit</trd>,
    <trd>spojovat</trd>,
    <trd>spol&ccaron;it</trd>
  </trdgrp>
  <trd lng="es">asociar</trd>
  <trd lng="fr">associer</trd>
  <trd lng="it">associare</trd>
  <trd
  lng="ru">&c_a;&c_s;&c_s;&c_o;&c_c;&c_i;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>spoji&tcaron;</trd>,
    <trd>spol&ccaron;i&tcaron;</trd>,
    <trd>zdru&zcaron;i&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="asoci.0ano">
  <kap><tld/>ito<fnt>Z</fnt>, <var><kap><tld/>ano</kap></var></kap>
  <snc>
    <dif>
      Membro de <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_l;&c_je;&c_c;
  <klr>(&c_a;&c_s;&c_a;&c_c;&c_y;&c_ja;&c_c;&c_y;&c_ib;,
  &c_s;&c_u;&c_p;&c_o;&c_l;&c_k;&c_ib; &c_ib; &c_d;&c_a;
  &c_g;.&c_p;.)</klr></trd>
  <trd lng="cs">&ccaron;len svazu</trd>
  <trd lng="en">associate</trd>
  <trdgrp lng="es">
    <trd>asociado</trd>,
    <trd>socio</trd>
  </trdgrp>
  <trd lng="fr">associ&eacute;<klr> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>t&aacute;rsas&aacute;g tagja</trd>,
    <trd>t&aacute;rsult (egy&eacute;n)</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>associato</trd>,
    <trd>sodale</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_l;&c_je;&c_n;<klr tip="amb">
  (&c_o;&c_b;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_a;,
  &c_a;&c_s;&c_s;&c_o;&c_c;&c_i;&c_a;&c_c;&c_i;&c_i; &c_i;
  &c_t;.&c_p;.)</klr></trd>
  <trd lng="sk">&ccaron;len zv&auml;zu</trd>
  <trd lng="th">&#3626;&#3617;&#3634;&#3594;&#3636;&#3585;&#3626;&#3617;&#3634;&#3588;&#3617;</trd>
</drv>

<drv mrk="asoci.0eca">
  <kap><tld/>eca</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="asoci.0eca.MAT">
    <subsnc mrk="asoci.0eca.operacio">
      <fnt><bib>MatTerm</bib><lok>p. 15</lok></fnt>
      <dif>
        <klr>(p.p. <ref cel="operac.0o.MAT">operacio</ref>
        <frm am="&#8224;">&#8224;</frm>
        en aro <frm am="bb E"><g>E</g></frm>)</klr>
        Tia, ke <frm am="(x&#8224;y)&#8224;z=x&#8224;(y&#8224;z)">(x&#8224;y)&#8224;z =
        x&#8224;(y&#8224;z)</frm> por &ccirc;iuj
        <frm am="x,y,z in bb E">x, y, z&#8712;<g>E</g></frm>:
        <ekz>
          la aritmetikaj operacioj adicio kaj multipliko estas
          <tld/>ecaj,
          subtraho kaj divido ne estas;
        </ekz>
        <ekz>
          estas iom tede demonstri la <ind><tld/>eco</ind>n de matrica
          multipliko.
          <trd
          lng="be">&c_a;&c_s;&c_a;&c_c;&c_y;&c_ja;&c_t;&c_y;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
          <trd lng="de">Assoziativit&auml;t</trd>
          <trd lng="en">associativity</trd>
          <trd lng="fr">associativit&eacute;</trd>
          <trd lng="pl">&lstroke;&aogonek;czno&sacute;&cacute;</trd>
          <trd
          lng="ru">&c_a;&c_s;&c_s;&c_o;&c_c;&c_i;&c_a;&c_t;&c_i;&c_v;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
        </ekz>
      </dif>
      <rim>
        Se la operacio estas <tld/>eca, la krampoj ne plu estas uzendaj
        kaj
        oni simple signas per
        <frm am="x&#8224;y&#8224;z">x&#8224;y&#8224;z</frm>
        la &ccirc;i-suprajn rezultojn.
      </rim>
    </subsnc>
    <subsnc mrk="asoci.0eca.grupoido">
      <dif>
        <klr>(p.p. <ref cel="grupoi.0o">grupoido</ref>)</klr>
        Tia, ke &gcirc;ia operacio estas <tld/>eca
        <sncref ref="asoci.0eca.operacio"/>.
      </dif>
    </subsnc>
  </snc>
  <trd
  lng="be">&c_a;&c_s;&c_a;&c_c;&c_y;&c_ja;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>
  <trd lng="de">assoziativ</trd>
  <trd lng="en">associative</trd>
  <trd lng="fr">associatif<klr tip="ind"> (math.)</klr></trd>
  <trd lng="hu">asszociat&iacute;v</trd>
  <trd lng="it">associativo<klr> (mat.)</klr></trd>
  <trd lng="pl">&lstroke;&aogonek;czny</trd>
  <trd
  lng="ru">&c_a;&c_s;&c_s;&c_o;&c_c;&c_i;&c_a;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="asoci.ide0o">
  <kap>ide<tld/>o</kap>
  <snc mrk="asoci.ide0o.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      Mensa interligo inter ideoj, pro kiu penso pri 
      la unua sekvigas penson pri la alia:
      <ekz>
        niaj ide<tld/>oj <klr>...</klr> fari&gcirc;as kutimaj per
        ripetado
        <fnt>
          <aut>C. Baudouin</aut>,
          <vrk>La Arto de Memdisciplino: Psikagogio</vrk>,
          <lok>1926</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">asociaci&oacute;n de ideas</trd>
  <trd lng="fr">association d'id&eacute;es</trd>
</drv>
</art>
