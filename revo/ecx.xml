<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>e&ccirc;</rad>
</kap>
<drv mrk="ecx.0">
  <kap><ofc>*</ofc><tld/></kap>
  <gra><vspec>adverbo</vspec></gra>
  <snc mrk="ecx.0.KOMUNE">
    <dif>
     Surprize anka&ubreve;; ne nur sed plie; sed krom tio:
     <ekz>
      kontentigi &ccirc;iujn <tld/> an&gcirc;elo ne
      povas<fnt><bib>PrV</bib></fnt>;
     </ekz>
     <ekz>
      <tld/> kiu plej bone pafas tamen iam
      maltrafas<fnt><bib>PrV</bib></fnt>;
     </ekz>
     <ekz>
      <tld/> sur la suno trovi&gcirc;as
      makuloj<fnt><bib>PrV</bib></fnt>;
     </ekz>
     <ekz>
      <tld/> signo ne restis, kie urbo estis<fnt><bib>PrV</bib></fnt>;
     </ekz>
     <ekz>
      kaj mi <tld/> diros ke...;
     </ekz>
     <ekz>
      <ind><tld/> se</ind> mi volus halti, mi ne povus<fnt>Z</fnt>;
      <trd lng="fi">vaikka</trd>
     </ekz>
     <ekz>
      mi ne diros <tld/> unu vorton<fnt>Z</fnt>
      <klr>(absolute neniun vorton)</klr>.
     </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_v;&c_a;&c_t;, &c_n;&c_a;&c_t;</trd> 
  <trdgrp lng="cs">
    <trd>ba i</trd>,
    <trd>dokonce</trd>
  </trdgrp>
  <trdgrp lng="de">
   <trd>sogar</trd>,
   <trd>selbst</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>incluso</trd>,
    <trd>aun</trd>
  </trdgrp>
  <trdgrp lng="fi">
    <trd>jopa</trd>,
    <trd>viel&auml;p&auml;</trd>,
    <trd>-<ind>kin</ind></trd>,
    <trd><klr>(en nea frazo) </klr> edes</trd>,
    <trd>-<ind>kaan, -k&auml;&auml;n</ind></trd>
  </trdgrp>
  <trd lng="fr">m&ecirc;me<klr tip="ind"> (adv.)</klr></trd>
  <trdgrp lng="he">
    <trd>&alef;&pe;&yod;&lamed;&vav;</trd>,
    <trd>&alef;&fpe;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>s&odblac;t</trd>,
    <trd>m&eacute;g ... is</trd>
  </trdgrp>
  <trd lng="nl">zelfs</trd>
  <trdgrp lng="pl">
    <trd>wr&eogonek;cz</trd>,
    <trd>nawet</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_a;&c_zh;&c_je;</trd>
  <trd lng="tp">kin</trd>
  <trd lng="tr">bile</trd>
</drv>

<drv mrk="ecx.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="ecx.0a.KOMUNE">
   <dif>
    Anka&ubreve;a, plia:
    <ekz>
      ni a&ubreve;skultis sen <tld/>a brueto dum la fajro preska&ubreve;
      estingi&gcirc;is
      <fnt>
       <aut>A. L&ouml;wenstein</aut>,
       <vrk>La &scirc;tona urbo</vrk>, 
       <lok>FEL, 1999</lok>
      </fnt>.
    </ekz>
   </dif>
  </snc>
</drv>
</art>
