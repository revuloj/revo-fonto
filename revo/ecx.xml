<art mrk="$Id: ecx.xml,v 1.48 2021/06/08 17:42:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>e&ccirc;</rad> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ecx.0">
  <kap><ofc>*</ofc><tld/></kap>
  <gra><vspec>adverbo</vspec></gra>
  <snc mrk="ecx.0.KOMUNE">
    <dif>
     Surprize anka&ubreve;; ne nur sed plie; sed krom tio:
     <ekz>
      kontentigi &ccirc;iujn <tld/> an&gcirc;elo ne
      povas<fnt><bib>PrV</bib></fnt>;
     </ekz>
     <ekz>
      <tld/> kiu plej bone pafas tamen iam
      maltrafas<fnt><bib>PrV</bib></fnt>;
     </ekz>
     <ekz>
      <tld/> sur la suno trovi&gcirc;as
      makuloj<fnt><bib>PrV</bib></fnt>;
     </ekz>
     <ekz>
      <tld/> signo ne restis, kie urbo estis<fnt><bib>PrV</bib></fnt>;
     </ekz>
     <ekz>
      kaj mi <tld/> diros ke...;
     </ekz>
     <ekz>
      mi ne diros <tld/> unu vorton<fnt>Z</fnt>
      <klr>(absolute neniun vorton)</klr>.
     </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="se.0ecx.ARG">se <tld/></ref>
    </refgrp>
  </snc>
  <trd lng="af">selfs</trd>
  <trd lng="am">&#x12A5;&#x1295;&#x12B3;&#x1295;</trd>
  <trd lng="ar">&#x62D;&#x62A;&#x649;</trd>
  <trd lng="az">h&#x259;tta</trd>
  <trd lng="be">&c_n;&c_a;&c_v;&c_a;&c_t;, &c_n;&c_a;&c_t;</trd> 
  <trd lng="bn">&#x98F;&#x9AE;&#x9A8;&#x995;&#x9BF;</trd>
  <trdgrp lng="cs">
    <trd>ba i</trd>,
    <trd>dokonce</trd>
  </trdgrp>
  <trd lng="da">selv</trd>
  <trdgrp lng="de">
   <trd>sogar</trd>,
   <trd>selbst</trd>
  </trdgrp>
  <trd lng="en">even</trd> 
  <trdgrp lng="es">
    <trd>incluso ("e&ccirc;")</trd>,
    <trd>aun ("e&ccirc;")</trd>,
    <trd>ni siquiera ("e&ccirc; ne")</trd>
  </trdgrp>
  <trd lng="et">isegi</trd>
  <trdgrp lng="fi">
    <trd>jopa</trd>,
    <trd>viel&auml;p&auml;</trd>,
    <trd>-<ind>kin</ind></trd>,
    <trd><klr>(en nea frazo) </klr> edes</trd>,
    <trd>-<ind>kaan, -k&auml;&auml;n</ind></trd>
  </trdgrp>
  <trd lng="fr">m&ecirc;me<klr tip="ind"> (adv.)</klr></trd>
  <trdgrp lng="gl">
    <trd>a&iacute;nda ("e&ccirc;")</trd>,
    <trd>incluso ("e&ccirc;")</trd>,
    <trd>inclusive ("e&ccirc;")</trd>,
    <trd>mesmo ("e&ccirc;")</trd>,
    <trd>nin sequera ("e&ccirc; ne")</trd>,
    <trd>nin tan sequera ("e&ccirc; ne")</trd>
  </trdgrp>
  <trd lng="gu">&#xAAA;&#xAA3;</trd>
  <trdgrp lng="he">
    <trd>&alef;&pe;&yod;&lamed;&vav;</trd>,
    <trd>&alef;&fpe;</trd>
  </trdgrp>
  <trd lng="hi">&#x914;&#x930; &#x92D;&#x940;</trd>
  <trdgrp lng="hu">
    <trd>s&odblac;t</trd>,
    <trd>m&eacute;g ... is</trd>
  </trdgrp>
  <trd lng="hy">&#x576;&#x578;&#x582;&#x575;&#x576;&#x56B;&#x57D;&#x56F;</trd>
  <trdgrp lng="it">
    <trd>persino ("e&ccirc;")</trd>,
    <trd>perfino ("e&ccirc;")</trd>,
    <trd>neanche ("e&ccirc; ne")</trd>,
    <trd>nemmeno ("e&ccirc; ne")</trd>,
    <trd>neppure ("e&ccirc; ne")</trd>
  </trdgrp>
  <trd lng="ja">&#x3055;&#x3089;&#x306B;</trd>
  <trd lng="ka">&#x10D9;&#x10D8;</trd>
  <trd lng="ko">&#xB3C4;</trd>
  <trd lng="mr">&#x905;&#x917;&#x926;&#x940;</trd>
  <trd lng="my">&#x1015;&#x1004;&#x103A;</trd>
  <trd lng="ne">&#x92A;&#x928;&#x93F;</trd>
  <trd lng="nl">zelfs</trd>
  <trd lng="pa">&#xA35;&#xA40;</trd>
  <trdgrp lng="pl">
    <trd>wr&eogonek;cz</trd>,
    <trd>nawet</trd>
  </trdgrp>
  <trd lng="ro">chiar</trd>
  <trd lng="ru">&c_d;&c_a;&c_zh;&c_je;</trd>
  <trd lng="sq">edhe</trd>
  <trd lng="te">&#xC15;&#xC42;&#xC21;&#xC3E;</trd>
  <trd lng="tl">kahit na</trd>
  <trd lng="tp">kin</trd>
  <trd lng="tr">bile</trd>
  <trd lng="ur">&#x628;&#x6BE;&#x6CC;</trd>
  <trd lng="vi">th&#x1EAD;m ch&#xED;</trd>
</drv>

<drv mrk="ecx.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="ecx.0a.KOMUNE">
   <dif>
    Anka&ubreve;a, plia:
    <ekz>
      ni a&ubreve;skultis sen <tld/>a brueto dum la fajro preska&ubreve;
      estingi&gcirc;is
      <fnt>
       <aut>A. L&ouml;wenstein</aut>,
       <vrk>La &scirc;tona urbo</vrk>, 
       <lok>1999</lok>
      </fnt>.
    </ekz>
   </dif>
  </snc>
  <trd lng="nl">enig</trd>
</drv>
