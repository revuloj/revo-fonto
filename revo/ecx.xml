<drv mrk="ecx.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="ecx.0a.KOMUNE">
    <dif>
      <ref tip="dif" cel="ankaux.0a">Anka&ubreve;a</ref>, <ref cel="pli.0a.aldona">plia</ref>;
      <tld/> plej malgranda:
      <ekz>
        ni a&ubreve;skultis sen <tld/>a brueto dum la fajro preska&ubreve;
        estingi&gcirc;is
        <fnt>
         <aut>A. L&ouml;wenstein</aut>,
         <vrk>La &scirc;tona urbo</vrk>,
         <lok>1999</lok>
        </fnt>;
      </ekz>
      <ekz>
        senpripensa pa&scirc;o sufi&ccirc;as por ke jen, oni retrovi&gcirc;u enretigita sen <tld/>a
        elirdeziro
        <fnt>
          <bib>Kon11</bib>
          <lok>E-Kulturo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="nur.0a">nura</ref>,
      <ref cel="sol.0a.neniukroma">sola<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_d;&c_a;&c_t;&c_k;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_l;&c_ib;&c_sh;&c_n;&c_ib;</trd>
  </trdgrp>
  <trd lng="de">den <ind>gering</ind>sten</trd>
  <trd lng="nl">enig</trd>
</drv>

