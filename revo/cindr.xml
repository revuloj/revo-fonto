<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>cindr</rad>/o
</kap>
<drv mrk="cindr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Grizkolora brulresta&jcirc;o:
      <ekz>
        <tld/>oj de ligno, karbo, papero, herboj.
      </ekz>
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Korporesta&jcirc;o de la mortintoj:
      <ekz>
        paco al lia <tld/>o
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Asche</trd>
</drv>
<drv mrk="cindr.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Meti per brulo en staton de <tld/>o, tute forbruligi:
      <ekz>
        domo <tld/>igita de brulego;
      </ekz>
      <ekz>
        <tld/>igi kadavron.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>niederbrennen</trd>,
    <trd>ein&auml;schern</trd>
  </trdgrp>
</drv>
<drv mrk="cindr.0ulino">
  <kap><tld/>ulino<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Malpura servistino.  
    </dif>
  </snc>
  <trd lng="de">Aschenputtel</trd>
</drv>
</art>
<!--
$Log$
-->
</vortaro>