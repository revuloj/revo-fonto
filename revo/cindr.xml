<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>cindr</rad>/o
</kap>
<drv mrk="cindr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Grizkolora brulresta&jcirc;o:
      <ekz>
        <tld/>oj de ligno, karbo, papero, herboj.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_p;&c_je;&c_p;&c_je;&c_l;</trd>,
      <trd>&c_z;&c_o;&c_l;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <dif>
      Korporesta&jcirc;o de la mortintoj:
      <ekz>
        paco al lia <tld/>o
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>!
      </ekz>
    </dif>
    <trd lng="ru">&c_p;&c_r;&c_a;&c_h;</trd>
  </snc>
  <trd lng="de">Asche</trd>
  <trd lng="fr">cendre(s)</trd>
</drv>
<drv mrk="cindr.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Meti per brulo en staton de <tld/>o, tute forbruligi:
      <ekz>
        domo <tld/>igita de brulego;
      </ekz>
      <ekz>
        <tld/>igi kadavron.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>niederbrennen</trd>,
    <trd>ein&auml;schern</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>incin&eacute;rer</trd>,
    <trd>r&eacute;duire en <ind>cendres</ind></trd>
  </trdgrp>
</drv>
<drv mrk="cindr.0ulino">
  <kap><tld/>ulino<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Malpura servistino.  
    </dif>
  </snc>
  <trd lng="de">Aschenputtel</trd>
  <trd lng="fr">Cendrillon</trd>
  <trd lng="ru">&c_z;&c_o;&c_l;&c_u;&c_sh;&c_k;&c_a;</trd>
</drv>
</art>
