<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: antici.xml,v 1.37 2025/10/08 22:00:22 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>anticip</rad>/i <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="antici.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="antici.0i.KOMUNE">
    <dif>
      Fari, plenumi, preni ion anta&ubreve; la konvena, deviga a&ubreve;
      kutima tempo;  diri, fidi ion anta&ubreve; &gcirc;ia fari&gcirc;o:
      <ekz>
        mi jam nun <tld/>as, ke <klr>[&#8230;]</klr>
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> per a&ubreve;daca esploro ekster la sciencaj normoj de sia epoko,
        <tld/>is gravajn fizikajn fenomenojn jardekojn anta&ubreve; ol ili estis efektive
        malkovritaj
        <fnt>
          <bib>Kon11</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiam oni <tld/>as balda&ubreve; morti, alarmo trafas onin kaj oni meditas pri aferoj
        kiujn oni anta&ubreve;e ne atentis
        <fnt>
          <bib>Rsp</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>as kun &gcirc;ojo pliajn kontribua&jcirc;ojn de la estimata redaktoro
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Blaskowicz</aut>
          <vrk><url ref="&Monato;/2005/008694.php">Amuza Gubbins</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili gaje <tld/>as la konsumotan vinon, kiu devas esti fortrinkita
        <fnt>
          <bib>Monato</bib>
          <aut>Judit Felszeghy</aut>
          <vrk><url ref="&Monato;/2007/009351.php">Je via sano!</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dir.antaux0i">anta&ubreve;diri</ref>,
      <ref cel="vid.antaux0i">anta&ubreve;vidi</ref>,
<ref cel="progno.0i">prognozi</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_a;&c_p;&c_ja;&c_r;&c_e;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_p;&c_ja;&c_r;&c_e;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_z;&c_a;&c_g;&c_a;&c_d;&c_z;&c_ja;</ind></trd>
    </trdgrp>
    <trdgrp lng="bg">
      <trd>&c_o;&c_ch;&c_a;&c_k;&c_v;&c_a;&c_m;</trd>,
      <trd>&c_i;&c_z;&c_p;&c_r;&c_je;&c_v;&c_a;&c_r;&c_v;&c_a;&c_m;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>vorwegnehmen</trd>,
      <trd>vorgreifen</trd>,
      <trd>antizipieren</trd>
    </trdgrp>
    <trd lng="en">anticipate</trd>
    <trd lng="es">anticipar</trd>
    <trd lng="fr">anticiper</trd>
    <trd lng="nl">anticiperen</trd>
    <trd lng="pl">antycypowa&cacute;</trd>
    <trd lng="ro">anticipa</trd>
    <trd lng="ms">menjangka</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_je;&c_d;&c_v;&c_o;&c_s;&c_h;&c_i;&c_shch;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_b;&c_je;&c_g;&c_a;&c_t;&c_mol; &c_v;&c_p;&c_je;&c_r;&c_jo;&c_d;</trd>,
      <trd>&c_p;&c_r;&c_je;&c_d;&c_v;&c_a;&c_r;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_p;&c_je;&c_r;&c_je;&c_zh;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>anticipova&tcaron;</trd>,
      <trd>predbieha&tcaron; <klr>(udalosti)</klr></trd>
    </trdgrp>
    <trd lng="sv">f&ouml;regripa</trd>
  </snc>
  <snc mrk="antici.0i.EKON">
    <fnt><bib>KrED</bib></fnt>
    <dif>
      <ref tip="dif" cel="pag.antaux0i">Anta&ubreve;pagi</ref>;
      <ref cel="avanc.0i.EKON">avanci<sncref/></ref>.
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_l;&c_a;&c_c;&c_ib;&c_c;&c_mol; &c_z;&c_a;&c_g;&c_a;&c_d;&c_z;&c_ja;</trd>,
      <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
      <ind>&c_p;&c_je;&c_r;&c_a;&c_d;&c_a;&c_p;&c_l;&c_a;&c_t;&c_u;</ind></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd><klr>im </klr>Voraus leisten</trd>,
      <trd>vorausbezahlen</trd>,
      <trd><klr>im </klr>Voraus <ind>bezahlen</ind></trd>
    </trdgrp>
  </snc>
  <trd lng="ca">anticipar</trd>
  <trdgrp lng="cs">
    <trd>anticipovat</trd>,
    <trd>p&rcaron;edj&iacute;mat</trd>,
    <trd>ur&ccaron;it p&rcaron;edem</trd>,
    <trd>u&ccaron;init p&rcaron;edem</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&lamed;&he;&qof;&dalet;&yod;&fmem;</trd>,
    <trd>&lamed;&tsadi;&pe;&vav;&tav;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#20808;&#12435;&#12376;&#12427;  <pr>&#12373;&#12365;&#12435;&#12376;&#12427;</pr></trd>,
    <trd>&#21069;&#12418;&#12387;&#12390;&#25163;&#37197;&#12377;&#12427;  <pr>&#12414;&#12360;&#12418;&#12387;&#12390;&#12390;&#12399;&#12356;&#12377;&#12427;</pr></trd>,
    <trd>&#35211;&#36234;&#12377;  <pr>&#12415;&#12371;&#12377;</pr></trd>,
    <trd>&#20104;&#26399;&#12377;&#12427;  <pr>&#12424;&#12365;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>adiantar</trd>,
    <trd>antecipar</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>predbieha&tcaron;</trd>,
    <trd>predch&aacute;dza&tcaron;</trd>,
    <trd>predn&iacute;ma&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_p;&c_je;&c_r;&c_je;&c_d;&c_zh;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_b;&c_ib;&c_g;&c_a;&c_t;&c_i; &c_v;&c_p;&c_je;&c_r;&c_je;&c_d;</trd>,
    <trd>&c_v;&c_i;&c_p;&c_je;&c_r;&c_je;&c_d;&c_zh;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="antici.0a">
  <kap><tld/>a</kap>
  <snc mrk="antici.0a.KOMUNE">
    <dif>
      Anta&ubreve;tempa:
      <ekz>
        malatento al nur unu el tiuj kvar elementoj signifas <tld/>an fiaskon
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>a &gcirc;ojo</ind> pri Kristnasko
        <fnt>
          <bib>FSI</bib>
        </fnt>;
        <trd lng="de">Vorfreude</trd>
      </ekz>
      <ekz>
        <ind><tld/>a registri&gcirc;o</ind>
        <fnt>
          <bib>Kon11</bib>
        </fnt>;
        <trd lng="de">Voranmeldung</trd>
      </ekz>
      <ekz>
        <tld/>a sinanonco estas konsilenda
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 4 (54)</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a interkonsento
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#8470;8-9</lok>
        </fnt>;
      </ekz>
      <ekz>
        se &scirc;i estus honeste dirinta, ke mankas mono al &scirc;i, ni senhezite estus donintaj
        <tld/>an pagon al &scirc;i
        <fnt>
          <bib>KrM</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#19981;&#36866;&#26102; <pr>b&ugrave;sh&igrave;sh&iacute;</pr></trd>,
    <trd>&#19981;&#36969;&#26178; <pr>b&ugrave;sh&igrave;sh&iacute;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_p;&c_je;&c_r;&c_a;&c_d;&c_zh;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_d;&c_a;&c_t;&c_e;&c_r;&c_m;&c_ib;&c_n;&c_o;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>vorzeitig</trd>,
    <trd>vorfristig</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>anticip&eacute;</trd>,
    <trd>pr&eacute;visionnel</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>in advance</trd>,
    <trd>ahead of time</trd>,
    <trd>preemptive</trd>,
    <trd>pre-emptive</trd>
  </trdgrp>
  <trd lng="he">&tsadi;&pe;&vav;&yod; &mem;&resh;&alef;&shin;</trd>
  <trdgrp lng="pl">
    <trd>przewiduj&aogonek;cy</trd>,
    <trd>uprzedzaj&aogonek;cy</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>adiantado</trd>,
    <trd>antecipado</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>previzibil&abreve;</trd>,
    <trd>anticip&acirc;nd</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_r;&c_o;&c_b;&c_l;&c_je;&c_n;&c_i;&c_j; &c_z;&c_a;&c_z;&c_d;&c_a;&c_l;&c_je;&c_g;&c_ib;&c_d;&c_mol;</trd>,
    <trd>&c_d;&c_o;&c_s;&c_t;&c_r;&c_o;&c_k;&c_o;&c_v;&c_o;</trd>,
    <trd>&c_p;&c_o;&c_p;&c_je;&c_r;&c_je;&c_d;&c_n;&c_ib;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="antici.0eal">
  <kap><tld/>e al</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="antici.0eal.KOMUNE">
    <dif>
      Iom da tempo
      <ref tip="super" cel="antaux.0.tempe">anta&ubreve;<sncref/></ref>;
      <ref tip="vid" cel="prepar.0eal.KOMUNE">prepare al</ref>:
      <ekz>
        <tld/>e al la kopenhaga UK
        <fnt>
          <bib>Kon11</bib>
        </fnt>;
      </ekz>
      <ekz>
        la laboristoj estos kiel eble plej pretaj kaj kiom eble plej 
        varbitaj al socialismo <tld/>e al la revolucio
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_g;&c_a;&c_d;&c_z;&c_ja;</trd>,
    <trd>&c_d;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_d;</trd>,
    <trd>&c_n;&c_a; <ind>&c_p;&c_je;&c_r;&c_a;&c_d;&c_p;&c_a;&c_ch;&c_a;&c_t;&c_k;&c_u;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>mit <ind>Aussicht</ind> auf</trd>,
    <trd>im <ind>Angesicht</ind> von</trd>,
    <trd>in <ind>Erwartung</ind> von</trd>
  </trdgrp>
  <trd lng="fr">en pr&eacute;vision de</trd>
  <trdgrp lng="pl">
    <trd>wpierw</trd>,
    <trd>z g&oacute;ry</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>&icirc;nt&acirc;i</trd>,
    <trd>&icirc;n prealabil</trd>
  </trdgrp>
</drv>
</art>
