<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: retroced.xml,v 1.5 2024/04/13 20:15:53 revo Exp $">
<kap>
  <rad>retroced</rad>/i
</kap>
<drv mrk="retroced.0i">
  <kap><tld/>i</kap>
  <snc mrk="retroced.0i.KOMUNE">
    <dif>
      Retroiri, cedante spacon:
      <ekz>
        kiu volas vidi la lumon / kia &gcirc;i estas /
        devas <tld/>i / en la ombron
        <fnt>
          <aut>H. M. Encensbergo, trad. V. Lutermano</aut>,
          <vrk><url ref="http://vlutermano.free.fr/Ence_poemoj2.html">
          Ombroregno</url></vrk>,
          <lok>1999</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_s;&c_t;&c_u;&c_p;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_s;&c_t;&c_u;&c_p;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>c&eacute;der du terrain</trd>,
    <trd>se <ind>replier</ind></trd>
  </trdgrp>
  <trd lng="pl">ust&aogonek;pi&cacute;</trd>
  <trdgrp lng="zh">
    <trd>&#x64A4;&#x9000; <pr>ch&#xE8;tu&#xEC;</pr></trd>,
    <trd>&#x9000;&#x5374; <pr>tu&#xEC;qu&#xE8;</pr></trd>,
    <trd>&#x9000;&#x537B; <pr>tu&#xEC;qu&#xE8;</pr></trd>
  </trdgrp>
</drv>
</art>

