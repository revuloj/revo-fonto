<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>venen</rad>/o
</kap>
<drv mrk="venen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="venen.0o.MED">
    <dif>
      Malutila substanco, tute a&ubreve; parte detruanta la funkciojn de la vivo:
      <ekz>
        fosforo estas mortiga <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la <tld/>o de vipuro, araneo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ja;&c_d;</trd>,
    <trd>&c_a;&c_t;&c_r;&c_u;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">ver&iacute;</trd>
  <trd lng="de">Gift</trd>
  <trd lng="el">&delta;&eta;&lambda;&eta;&tau;&eta_ton;&rho;&jota;&omikron;</trd>
  <trd lng="en">poison</trd>
  <trd lng="es">veneno</trd>
  <trdgrp lng="fr">
    <trd>poison<klr> (subst.)</klr></trd>,
    <trd>venin</trd>
  </trdgrp>
  <trd lng="hu">m&eacute;reg</trd>  
  <trd lng="nl">vergif</trd>
  <trd lng="pt">veneno</trd>
  <trdgrp lng="ru">
    <trd>&c_ja;&c_d;</trd>,
    <trd>&c_o;&c_t;&c_r;&c_a;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">jed</trd>
  <trd lng="tr">zehir</trd>
</drv>

<drv mrk="venen.0a">
  <kap><tld/>a</kap>
  <snc mrk="venen.0a.MED">
    <dif>
      Enhavanta <tld/>on:
      <ekz>
        <tld/>a besto, vegeta&jcirc;o.
      </ekz>
    </dif>
  </snc>
  <snc mrk="venen.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Dan&gcirc;era kiel <tld/>o:
      <ekz>
        li estas tre <tld/>a kontra&ubreve; mi<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>aj paroloj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a influo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ja;&c_d;&c_a;&c_v;&c_ib;&c_t;&c_y;</trd>,
    <trd>&c_a;&c_t;&c_r;&c_u;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">verin&oacute;s</trd>
  <trd lng="de">giftig</trd>
  <trd lng="el">&delta;&eta;&lambda;&eta;&tau;&eta;&rho;&jota;&omega_ton;&delta;&eta;&sigma_fina;</trd>
  <trd lng="en">poisonous</trd>
  <trd lng="es">venenoso</trd>
  <trdgrp lng="fr">
    <trd>toxique</trd>,
    <trd>v&eacute;n&eacute;neux</trd>,
    <trd>venimeux</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>m&eacute;rgez&odblac;</trd>,
    <trd>m&eacute;rges</trd>
  </trdgrp>
  <trd lng="nl">giftig</trd>
  <trd lng="pt">venenoso</trd>
  <trd lng="ru">&c_ja;&c_d;&c_o;&c_v;&c_i;&c_t;&c_y;&c_j;</trd>
  <trd lng="sk">jedovat&yacute;</trd>
</drv>

<drv mrk="venen.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Mortigi per <tld/>o:
      <ekz>
        <tld/>i rabian hundon.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Miksi <tld/>on en ion:
      <ekz>
        mi <tld/>us la oceanon, por ke ili trinku morton el &ccirc;iuj
        fontoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la vino <tld/>ita<fnt>Z</fnt>;
      </ekz>
      <ekz>
        spir<tld/>i
        <klr>(pp gasoj)</klr>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="venen.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malagrabligi, malfeli&ccirc;igi:
      <ekz>
        donacon reprenu kaj mian vivon ne <tld/>u<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiu penso <tld/>as al mi la vivon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        vi <tld/>as mian maljunecon<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i la plezuron<fnt>Z</fnt>,
      </ekz>
      <ekz>
        &gcirc;uadon;
      </ekz>
      <ekz>
        polvo <tld/>as la spiradon<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>itaj &gcirc;ojoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la interregnaj rilatoj estas <tld/>ataj de la problemo de la
        naciecoj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="difekt">difekti</ref>,
      <ref cel="fusx.0i">fu&scirc;i</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_t;&c_r;&c_u;&c_c;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="ca">enverinar</trd>
  <trd lng="de">vergiften</trd>
  <trd lng="en">poison</trd>
  <trd lng="el">&delta;&eta;&lambda;&eta;&tau;&eta;&rho;&jota;&alfa_ton;&zeta;&omega;</trd>
  <trd lng="es">envenenar</trd>
  <trd lng="fr">empoisonner</trd>
  <trdgrp lng="hu">
    <trd>m&eacute;rgez</trd>,
    <trd>megm&eacute;rgez</trd>
  </trdgrp>
  <trd lng="nl">vergiftigen</trd>
  <trd lng="pt">envenenar</trd>
  <trd lng="ru">&c_o;&c_t;&c_r;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">otr&aacute;vi&tcaron;</trd>
</drv>
</art>
