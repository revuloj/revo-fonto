<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: timele.xml,v 1.14 2024/01/18 21:22:51 revo Exp $">
<kap>
  <rad>timeleac</rad>/oj
</kap>
<drv mrk="timele.0oj">
  <kap><tld/>oj</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref> de <ref
      tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Thymelaeaceae</trd>)
      de plej ofte lignecaj specioj kun tre fibra &scirc;elo,
      ofte malagrable odorantaj:
      <ekz>
        <ref tip="prt" cel="dafn.0o">dafno</ref>
        estas vasta genro de plantoj el familio <tld/>oj,
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=29734">Dafno</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">timele&agrave;cies</trd>
  <trd lng="de">Seidelbastgew&auml;chse</trd>
  <trd lng="fr">Thym&eacute;l&eacute;ac&eacute;es</trd>
  <trd lng="hu">boroszl&aacute;nf&eacute;l&eacute;k</trd>
  <trd lng="nl">peperboompjesfamilie</trd>
  <trd lng="pl">wawrzynkowate</trd>
  <trd lng="pt">timele&aacute;ceas</trd>
  <trdgrp lng="ru">
    <trd>&c_ja;&c_g;&c_o;&c_d;&c_k;&c_o;&c_v;&c_y;&c_je;<klr tip="ind"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_v;&c_o;&c_l;&c_ch;&c_n;&c_i;&c_k;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="uk">&c_t;&c_i;&#x43C;&c_je;&#x43B;&c_je;&c_jeu;&c_v;&c_ib;</trd>
</drv>
</art>
