<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pampelmus.xml,v 1.15 2024/02/16 19:24:10 revo Exp $">
<kap>
  <rad>pampelmus</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="pampelmus.0o">
  <kap><tld/>o</kap>
  <snc mrk="pampelmus.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="frukt.0o.BOT">Frukto</ref> de <tld/>arbo, granda, flav&scirc;ela, 
      kun gusto acida kaj iom amara:
      <ekz>
        senha&ubreve;tigu la oran&gcirc;ojn kaj la <tld/>on
        <fnt>
          <aut>red. A. Fagot</aut>,
          <vrk><url ref="http://fagot.alain.free.fr/kuirado/Rpf12/html/VbS01.html">
          A&ubreve;tuna salato</url></vrk>,
          <lok>Kelkaj receptoj, 2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[kiprajn]</klr> <tld/>ojn kaj aliajn citrusojn oni trovas jam en tute&ubreve;ropaj
        fruktovendejoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2011/010247.php">Kipro eksportas, &Ccirc;inio
          importas</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
      <trd lng="be">&c_p;&c_a;&c_m;&c_e;&c_l;&c_a; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
     <trd lng="nl">pompelmoes<klr tip="amb"> (vrucht)</klr></trd>
  </snc>
  <snc mrk="pampelmus.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <ref tip="dif" cel="pampelmus.0arbo.BOT"><tld/>arbo</ref>
   <trd lng="be">&c_p;&c_a;&c_m;&c_e;&c_l;&c_a; <klr>(&c_d;&c_r;&c_e;&c_v;&c_a;)</klr></trd>
    <trd lng="nl">pompelmoes<klr tip="amb"> (boom)</klr></trd>
  </snc>
  <rim mrk="pampelmus.0o.RIM">
    <tld/>o kaj la kruci&gcirc;o kun oran&gcirc;o, <ref tip="vid" cel="grapfrukt.0o">grapfrukto</ref>, tre 
    intersimilas.  Komuna uzo en pluraj lingvoj ne distingas ilin.
  </rim>
  <trd lng="cs">grapefruit</trd>  
  <trd lng="de">Pempelmuse</trd>
  <trd lng="en">pomelo</trd>
  <trd lng="fr">pamplemousse</trd>
  <trdgrp lng="ja">
    <trd>&#x30B6;&#x30DC;&#x30F3;</trd>,
    <trd>&#x30D6;&#x30F3;&#x30BF;&#x30F3;</trd>
  </trdgrp>
  <trd lng="pl">pomelo</trd>
  <trd lng="pt">toranja</trd>
  <trd lng="ro">pomelo</trd>
  <trd lng="sk">grapefruit</trd>
  <trdgrp lng="sv">
    <trd>pompelmus</trd>,
    <trd>pomelo</trd>
  </trdgrp>
   <trd lng="uk">&c_g;&c_r;&c_je;&c_j;&#x43F;&c_f;&c_r;&c_u;&c_t; (&#x43F;&#x43B;&c_ib;&c_d;)</trd>
  <trdgrp lng="zh">
    <trd>&#x6587;&#x65E6; <pr>w&#xE9;nd&#xE0;n</pr></trd>,
    <trd>&#x67DA;&#x5B50; <pr>y&#xF2;uzi</pr></trd>,
    <trd>&#x6935; <pr>ji&#x1CE;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pampelmus.0arbo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="pampelmus.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="citrus.0o.BOT">Citruso</ref> el Azio
      (<trd lng="la">Citrus maxima</trd>).
    </dif>
    <bld lok="&WCU;/thumb/7/7d/Starr_070313-5664_Citrus_maxima.jpg/360px-Starr_070313-5664_Citrus_maxima.jpg" prm="CC BY 3.0">frukto kaj folioj de <tld/>ujo (Citrus maxima), Maui, Pulehu
        <fnt>
          <aut>Forest &amp; Kim Starr, Plants of Hawaii, Image 070313-5664 from
          http://www.hear.org/starr/plants/images/image/?q=070313-5664</aut>
          <lok><url ref="&WCW;/File:Starr_070313-5664_Citrus_maxima.jpg">&WCW;/File:Starr_070313-5664_Citrus_maxima.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_m;&c_e;&c_l;&c_a; <klr>(&c_d;&c_r;&c_e;&c_v;&c_a;)</klr></trd>
  <trd lng="de">Pampelmusenbaum</trd>
  <trd lng="fr">pamplemoussier</trd>
  <trdgrp lng="ja">
    <trd>&#x30B6;&#x30DC;&#x30F3;</trd>,
    <trd>&#x30D6;&#x30F3;&#x30BF;&#x30F3;</trd>
  </trdgrp>
  <trd lng="nl">pompelmoes<klr tip="amb"> (boom)</klr></trd>
  <trdgrp lng="pl">
    <trd>pomara&nacute;cza olbrzymia</trd>,
    <trd>pomelo</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>toranja</trd>,
    <trd>toranjeira</trd>,
    <trd>pomelo</trd>
  </trdgrp>
  <trd lng="ro">pomelo</trd>
  <trd lng="sv">pompelmustr&auml;d</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_r;&c_je;&c_j;&#x43F;&c_f;&c_r;&c_u;&c_t;</trd>,
    <trd>&#x43F;&c_a;&#x43C;&#x43F;&c_je;&#x43B;&#x44C;&#x43C;&c_u;&c_s;</trd>
  </trdgrp>
  <trd lng="zh">&#x67DA; <pr>y&#xF2;u</pr></trd>
</drv>
</art>

