<art mrk="$Id$">
<kap>
  <rad>altan</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="altan.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Artefarita ebena teraso, kiu superas la &ccirc;irka&ubreve;a&jcirc;on.
    </dif>
    <trd lng="de">Aussichtsterrasse</trd>
    <trd lng="fr">terrasse panoramique</trd>
    <trd lng="pt">palanque</trd>
  </snc>
  <snc>
    <dif>
      Belvidejo.
    </dif>
    <trd lng="de">Aussichtspunkt</trd>
    <trd lng="fr">belv&eacute;d&egrave;re</trd>
    <trdgrp lng="pt">
      <trd>mirante</trd>,
      <trd>belveder</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_b;&c_e;&c_l;&c_mol;&c_v;&c_e;&c_d;&c_e;&c_r;</trd>
  <trd lng="nl">uitkijkpunt</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_mol;&c_v;&c_je;&c_d;&c_je;&c_r;</trd>
  <trd lng="fr">overlook</trd>
</drv>
</art>
