<art mrk="$Id: altan.xml,v 1.21 2023/10/12 18:05:09 revo Exp $">
<kap>
  <rad>altan</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="altan.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="super" cel="teras.0o">Teraso</ref>, elrigardejo, superanta la &ccirc;irka&ubreve;on, 
      plej ofte kun tegmento:
      <ekz>
        &gcirc;ardena <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        en <klr>la urbo</klr> estis videblaj similaj formita&jcirc;oj
        kun turoj kaj kupoloj kaj velformaj <tld/>oj
        <fnt><bib>Fab1</bib>, <lok>Galo&scirc;oj de feli&ccirc;o</lok></fnt>;
      </ekz>
      <ekz>
        sur &ccirc;iu balkono kaj <tld/>o turnita suden vidi&gcirc;as homaj korpoj, palaj kaj
        senmovaj kiel mortintoj
        <fnt>
          <bib>LOdE</bib>
          <lok> 2003, &#x2116; 8-9 (106-107)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="belved.0o">belvedero</ref>,
      <ref cel="vid.0ejo">videjo</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Aussichtsterrasse</trd>,
      <trd>Altan</trd>,
      <trd>S&ouml;ller</trd>,
      <trd>Austritt</trd>
    </trdgrp>
    <trd lng="fr">terrasse panoramique</trd>
    <trd lng="hu">f&ouml;ldterasz</trd>
    <trd lng="pt">palanque</trd>
    <trd lng="it">altana</trd>
  </snc>
