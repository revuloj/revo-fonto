<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>altan</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="altan.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Teraso superanta la &ccirc;irka&ubreve;on, plej ofte kun tegmento:
      <ekz>
        en <klr>la urbo</klr> estis videblaj similaj formita&jcirc;oj
        kun turoj kaj kupoloj kaj velformaj <tld/>oj
        <fnt><bib>Fab1</bib>, <lok>Galo&scirc;oj de feli&ccirc;o</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Aussichtsterrasse</trd>
    <trd lng="fr">terrasse panoramique</trd>
    <trd lng="hu">f&ouml;ldterasz</trd>
    <trd lng="pt">palanque</trd>
    <trd lng="it">altana</trd>
  </snc>
  <snc>
    <dif>
      Belvidejo.
    </dif>
    <trd lng="de">Aussichtspunkt</trd>
    <trd lng="fr">belv&eacute;d&egrave;re</trd>
    <trdgrp lng="hu">
      <trd>tet&odblac;terasz</trd>,
      <trd>altana</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>mirante</trd>,
      <trd>belveder</trd>
    </trdgrp>
   <trd lng="it">belvedere</trd>
  </snc>
  <trd lng="be">&c_b;&c_e;&c_l;&c_mol;&c_v;&c_e;&c_d;&c_e;&c_r;</trd>
  <trd lng="nl">uitkijkpunt</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_mol;&c_v;&c_je;&c_d;&c_je;&c_r;</trd>
  <trd lng="en">overlook</trd>
</drv>
</art>
