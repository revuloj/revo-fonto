  <trd lng="be">&c_b;&c_e;&c_l;&c_mol;&c_v;&c_e;&c_d;&c_e;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>alt&aacute;n</trd>,
    <trd>bes&iacute;dka</trd>
  </trdgrp>
  <trd lng="en">overlook</trd>
  <trd lng="he">&mem;&#x5B4;&resh;&pe;&#x5BC;&#x5B6;&samekh;&#x5B6;&tav; &tav;&tsadi;&pe;&yod;&tav;</trd>
  <trdgrp lng="ja">
    <trd>&#x898B;&#x6674;&#x3089;&#x3057;&#x53F0;  <pr>&#x307F;&#x306F;&#x3089;&#x3057;&#x3060;&#x3044;</pr></trd>,
    <trd>&#x5C55;&#x671B;&#x53F0;  <pr>&#x3066;&#x3093;&#x307C;&#x3046;&#x3060;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">uitkijkpunt</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_mol;&c_v;&c_je;&c_d;&c_je;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>rozh&lcaron;ad&ncaron;a</trd>,
    <trd>vyv&yacute;&scaron;en&aacute; terasa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x431;&#x435;&#x43B;&#x44C;&#x432;&#x435;&#x434;&#x435;&#x440;</trd>,
    <trd>&#x442;&#x435;&#x440;&#x430;&#x441;&#x430;</trd>,
    <trd>&#x430;&#x43B;&#x44C;&#x442;&#x430;&#x43D;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>
