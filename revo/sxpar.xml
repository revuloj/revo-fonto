<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>&scirc;par</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="sxpar.0i">
   <kap><ofc>*</ofc><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc mrk="sxpar.0i.kolekti">
     <dif>
       Kolekti provizon de io, precipe de mono, danke al
       modera elspezo a&ubreve; uzo:
       <ekz>
         <tld/>u gro&scirc;on, vi havos plenan po&scirc;on
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         <tld/>u kiam bone, vi havos kiam bezone
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         <ind>vivi de <tld/>ita kaso</ind>
         <trd lng="fr">vivre sur ses r&eacute;serves</trd>
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         <tld/>i provizojn dum sie&gcirc;o, soldatojn en milito;
       </ekz>
       <ekz>
         <uzo tip="stl">FIG</uzo>
         <tld/>u vian bonan vorton por amato de la dioj<fnt>Z</fnt>
         <klr></klr>.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="rezerv.0i">rezervi</ref>
     </refgrp>
     <trd lng="hu">megtakar&iacute;t</trd>
   </snc>
   <snc mrk="sxpar.0i.moderi">
     <dif>
       Modere kaj singarde uzi:
       <ekz>
         vortojn <tld/>u, agojn faru
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         <tld/>i nek penon nek admonon<fnt>Z</fnt>;
       </ekz>
       <ekz>
         <tld/>i siajn fortojn;
       </ekz>
       <ekz>
         kiu <tld/>as sian vergon, tiu malamas sian
         filon<fnt>Z</fnt>;
       </ekz>
       <ekz>
         tiuj <tld/>as por ni la pensadon;
       </ekz>
       <ekz>
         oni ne <tld/>is al li la apla&ubreve;dojn.
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="domagx.0i.avari">doma&gcirc;i</ref>
     </refgrp>
     <trd lng="en">be sparing of</trd>
     <trd lng="hu">takar&eacute;koskodik</trd>
   </snc>
   <trdgrp lng="be">
     <trd>&c_a;&c_sh;&c_ch;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_z;&c_a;&c_h;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_z;&c_mol;&c_b;&c_je;&c_r;&c_a;&c_g;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_e;&c_k;&c_a;&c_n;&c_o;&c_m;&c_ib;&c_c;&c_mol;</trd>
   </trdgrp>
   <trd lng="de">sparen</trd>
   <trdgrp lng="en">
     <trd>spare</trd>,
     <trd>save</trd>,
     <trd>keep for future use</trd>
   </trdgrp>
   <trd lng="es">ahorrar</trd>
   <trdgrp lng="fr">
     <trd>&eacute;conomiser</trd>,
     <trd>&eacute;pargner</trd>
   </trdgrp>
   <trdgrp lng="it">
     <trd>economizzare</trd>,
     <trd>risparmiare</trd>
   </trdgrp>
   <trd lng="nl">sparen</trd>
   <trdgrp lng="pt">
     <trd>economizar</trd>,
     <trd>poupar</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_s;&c_b;&c_je;&c_r;&c_je;&c_g;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_e;&c_k;&c_o;&c_n;&c_o;&c_m;&c_i;&c_t;&c_mol;</trd>
   </trdgrp>
   <trd lng="sv">spara</trd>
</drv>

<drv mrk="sxpar.0ema">
   <kap><tld/>ema</kap>
   <snc mrk="sxpar.0ema.KOMUNE">
     <dif>
       Ne elspezema.
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_a;&c_sh;&c_ch;&c_a;&c_d;&c_n;&c_y;</trd>,
     <trd>&c_b;&c_je;&c_r;&c_a;&c_zh;&c_l;&c_ib;&c_v;&c_y;</trd>,
     <trd>&c_e;&c_k;&c_a;&c_n;&c_o;&c_m;&c_n;&c_y;</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>sparsam</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>saving</trd>,
     <trd>economical</trd>,
     <trd>frugal</trd>,
     <trd>thrifty</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>&eacute;conome</trd>
   </trdgrp>
   <trd lng="hu">takar&eacute;kos</trd>
   <trd lng="nl">zuinig</trd>
   <trd lng="ru">&c_b;&c_je;&c_r;&c_je;&c_zh;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
</drv>

<drv mrk="sxpar.0igi">
   <kap><tld/>igi, <var><kap><tld/>i</kap></var></kap>
   <snc mrk="sxpar.0igi.FIG">
     <uzo tip="stl">FIG</uzo>
     <dif>
       Evitigi ion malbonan al iu:
       <ekz>
         ni volis <tld/>i al li tiun dolorigan baton;
       </ekz>
       <ekz>
         la ma&scirc;inoj <tld/>as al ni penadon kaj pensadon;
       </ekz>
       <ekz>
         <tld/>igi al iu larmojn.
       </ekz>
     </dif>
   <trdgrp lng="be">
     <trd>&c_z;&c_mol;&c_b;&c_je;&c_r;&c_a;&c_g;&c_ch;&c_y;</trd>,
     <trd>&c_u;&c_p;&c_ib;&c_l;&c_mol;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_u;&c_s;&c_mol;&c_c;&c_je;&c_r;&c_a;&c_g;&c_ch;&c_y;</trd>
   </trdgrp>
   <trd lng="de">ersparen</trd>
   <trd lng="en">save</trd>
   <trdgrp lng="fr">
     <trd>&eacute;pargner<klr> (quelque chose &agrave; 
quelqu'un)</klr></trd>
   </trdgrp>
   <trd lng="hu">megk&iacute;m&eacute;l<klr tip="ind"> (vkit 
vmit&odblac;l)</klr></trd>
   <trdgrp lng="it">
     <trd>evitare<klr> (qualcosa a qualcuno)</klr></trd>,
     <trd>risparmiare<klr> (far evitare)</klr></trd>
   </trdgrp>
   <trd lng="nl">besparen</trd>
   <trd lng="pt">poupar</trd>
   <trd lng="ru">&c_u;&c_b;&c_je;&c_r;&c_je;&c_ch;&c_mol;</trd>
   <trd lng="sv">bespara</trd>
   </snc>
</drv>

<drv mrk="sxpar.mal0i">
   <kap>mal<tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc mrk="sxpar.mal0i.KOMUNE">
     <dif>
       Elspezi multe kaj senprudente:
       <ekz>
         mal<tld/>ulo &gcirc;uas nelonge, avarulo neniam
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         por kio ni vane mal<tld/>as tempon
         <fnt><aut>Ivan Efremov, trad. Jurij Finkel</aut>, <vrk>
<url ref="http://www.esperanto.mv.ru/Efremov">Horo de Bovo</url></vrk>, 
<lok>&ccirc;apitro 5a</lok></fnt>.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="glit">elgliti</ref>,
       <ref cel="gren.0o">greno</ref>,
       <ref cel="tru.0o">truo</ref>
     </refgrp>
   </snc>
   <trdgrp lng="be">
     <trd>&c_m;&c_a;&c_r;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_m;&c_a;&c_r;&c_n;&c_a; &c_t;&c_r;&c_a;&c_c;&c_ib;&c_c;&c_mol;</trd>,
     <trd>&c_m;&c_a;&c_r;&c_n;&c_a;&c_t;&c_r;&c_a;&c_v;&c_ib;&c_c;&c_mol;</trd>,
     <trd>&c_r;&c_a;&c_s;&c_t;&c_r;&c_a;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_r;&c_a;&c_s;&c_t;&c_r;&c_a;&c_n;&c_zh;&c_y;&c_r;&c_y;&c_c;&c_mol;</trd>
   </trdgrp>
   <trd lng="de">verschwenden</trd>
   <trdgrp lng="en">
     <trd>waste</trd>,
     <trd>squander</trd>,
     <trd>dissipate</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>dilapider</trd>,
     <trd>gaspiller</trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>t&eacute;kozol</trd>,
     <trd>pazarol</trd>
   </trdgrp>
   <trdgrp lng="it">
     <trd>sprecare<klr> (specie di forze o energia)</klr></trd>,
     <trd>sperperare</trd>,
     <trd>prodigare<klr> (non risparmiare)</klr></trd>,
     <trd><ind>spendere</ind> e spandere</trd>
   </trdgrp>
   <trdgrp lng="nl">
     <trd>verkwisten</trd>,
     <trd>verkwanselen</trd>
   </trdgrp>
   <trdgrp lng="pt">
     <trd>gastar</trd>,
     <trd>desperdi&ccedil;ar</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_t;&c_r;&c_a;&c_n;&c_zh;&c_i;&c_r;&c_i;&c_t;&c_mol;</trd>,
     <trd>&c_t;&c_r;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>,
     <trd>&c_r;&c_a;&c_s;&c_t;&c_o;&c_ch;&c_a;&c_t;&c_mol;</trd>
   </trdgrp>
   <trd lng="sv">sl&ouml;sa</trd>
</drv>
</art>
