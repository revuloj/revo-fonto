<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxpar.xml,v 1.43 2023/12/03 01:39:15 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>&scirc;par</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="sxpar.0i">
   <kap><ofc>*</ofc><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc mrk="sxpar.0i.kolekti">
     <dif>
       Kolekti provizon de io, precipe de mono, danke al
       modera elspezo a&ubreve; uzo:
       <ekz>
         <tld/>u gro&scirc;on, vi havos plenan po&scirc;on
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         <tld/>u kiam bone, vi havos kiam bezone
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         <ind>vivi de <tld/>ita kaso</ind>
         <trd lng="fr">vivre sur ses r&eacute;serves</trd>
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         <tld/>i provizojn dum sie&gcirc;o, soldatojn en milito;
       </ekz>
       <ekz>
         <uzo tip="stl">FIG</uzo>
         <tld/>u vian bonan vorton por amato de la dioj<fnt>Z</fnt>
         <klr/>.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="rezerv.0i">rezervi</ref>
     </refgrp>
     <trd lng="hu">megtakar&iacute;t</trd>
     <trdgrp lng="pl">
       <trd>oszcz&eogonek;dza&cacute;</trd>,
       <trd>ciu&lstroke;a&cacute;</trd>
     </trdgrp>
   </snc>
   <snc mrk="sxpar.0i.moderi">
     <dif>
       Modere kaj singarde uzi;
       <ref tip="sin" cel="domagx.0i.avari">doma&gcirc;i</ref>:
       <ekz>
         vortojn <tld/>u, agojn faru
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         <tld/>i nek penon nek admonon<fnt>Z</fnt>;
       </ekz>
       <ekz>
         <tld/>i siajn fortojn;
       </ekz>
       <ekz>
         kiu <tld/>as sian vergon, tiu malamas sian
         filon<fnt>Z</fnt>;
       </ekz>
       <ekz>
         tiuj <tld/>as por ni la pensadon;
       </ekz>
       <ekz>
         oni ne <tld/>is al li la apla&ubreve;dojn.
       </ekz>
     </dif>
     <trd lng="en">to be sparing of</trd>
     <trd lng="hu">takar&eacute;koskodik</trd>
     <trd lng="pl">oszcz&eogonek;dza&cacute;</trd>
   </snc>
   <trdgrp lng="be">
     <trd>&c_a;&c_sh;&c_ch;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_z;&c_a;&c_h;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_z;&c_mol;&c_b;&c_je;&c_r;&c_a;&c_g;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_e;&c_k;&c_a;&c_n;&c_o;&c_m;&c_ib;&c_c;&c_mol;</trd>
   </trdgrp>
   <trd lng="de">sparen</trd>
   <trdgrp lng="en">
     <trd>to spare</trd>,
     <trd>save</trd>,
     <trd>keep for future use</trd>
   </trdgrp>
   <trd lng="es">ahorrar</trd>
   <trdgrp lng="fr">
     <trd>&eacute;conomiser</trd>,
     <trd>&eacute;pargner</trd>
   </trdgrp>
   <trdgrp lng="it">
     <trd>economizzare</trd>,
     <trd>risparmiare</trd>
   </trdgrp>
   <trd lng="nl">sparen</trd>
   <trdgrp lng="pt">
     <trd>economizar</trd>,
     <trd>poupar</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_s;&c_b;&c_je;&c_r;&c_je;&c_g;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_e;&c_k;&c_o;&c_n;&c_o;&c_m;&c_i;&c_t;&c_mol;</trd>
   </trdgrp>
   <trd lng="sv">spara</trd>
</drv>

<drv mrk="sxpar.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="sxpar.0ajxo.EKON">
    <dif>
      Tio, kion oni <tld/>is:
      <ekz>
        la lastaj <tld/>a&jcirc;oj jam konsumi&gcirc;is
        <fnt>
          <aut>F. Ko&zcaron;&iacute;k, trad. J. Karen kaj J. Vondrou&scaron;ek</aut>,
          <vrk>La plej eminenta inter Pierotoj</vrk>,
          <lok>2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_sh;&c_ch;&c_a;&c_d;&c_zh;&c_a;&c_n;&c_n;&c_je; <klr>(&c_s;&c_r;&c_o;&c_d;&c_k;&c_ib;)</klr></trd>
  <trdgrp lng="fr">
    <trd>&eacute;conomies<klr> (&eacute;pargne)</klr></trd>,
    <trd>&eacute;pargne</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x305F;&#x304F;&#x308F;&#12360;</trd>,
    <trd>&#x8CAF;&#x84C4;  <pr>&#12385;&#12423;&#12385;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="pl">oszcz&eogonek;dno&sacute;ci</trd>
  <trd lng="uk">&c_z;&c_a;&#x43E;&c_shch;&c_a;&c_d;&c_zh;&c_je;&#x43D;&#x43D;&#x44F;</trd>
</drv>

<drv mrk="sxpar.0ema">
   <kap><ofc>*</ofc><tld/>ema</kap>
   <snc mrk="sxpar.0ema.KOMUNE">
     <dif>
       Ne elspezema:
      <ekz>
        Lia edzino estas tre laborema kaj <tld/>ema, sed &scirc;i estas anka&ubreve;
        tre babilema kaj kriema
        <fnt><bib>F</bib>, <lok>&FE; 41</lok></fnt>.
      </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_a;&c_sh;&c_ch;&c_a;&c_d;&c_n;&c_y;</trd>,
     <trd>&c_b;&c_je;&c_r;&c_a;&c_zh;&c_l;&c_ib;&c_v;&c_y;</trd>,
     <trd>&c_e;&c_k;&c_a;&c_n;&c_o;&c_m;&c_n;&c_y;</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>sparsam</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>saving</trd>,
     <trd>economical</trd>,
     <trd>frugal</trd>,
     <trd>thrifty</trd>
   </trdgrp>
   <trd lng="es">ahorrador</trd>
   <trd lng="fr">&eacute;conome<klr> (adj.)</klr></trd>
   <trd lng="hu">takar&eacute;kos</trd>
   <trdgrp lng="ja">
    <trd>&#12388;&#x307E;&#12375;&#12356;</trd>,
    <trd>&#x7BC0;&#x7D04;&#x5BB6;&#x306E;  <pr>&#x305B;&#12388;&#12420;&#x304F;&#x304B;&#x306E;</pr></trd>,
    <trd>&#29289;&#x60DC;&#12375;&#x307F;&#12377;&#x308B;  <pr>&#12418;&#x306E;&#x304A;&#12375;&#x307F;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">zuinig</trd>
   <trd lng="pl">oszcz&eogonek;dny</trd>
   <trd lng="ru">&c_b;&c_je;&c_r;&c_je;&c_zh;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
  <trd lng="uk">&#x43E;&c_shch;&c_a;&c_d;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>
</drv>

<drv mrk="sxpar.0igi">
   <kap><tld/>igi, <var><kap><tld/>i</kap></var></kap>
   <snc mrk="sxpar.0igi.FIG">
     <uzo tip="stl">FIG</uzo>
     <dif>
       Evitigi ion malbonan al iu:
       <ekz>
         ni volis <tld/>i al li tiun dolorigan baton;
       </ekz>
       <ekz>
         la ma&scirc;inoj <tld/>as al ni penadon kaj pensadon;
       </ekz>
       <ekz>
         <tld/>igi al iu larmojn.
       </ekz>
     </dif>
   <trdgrp lng="be">
     <trd>&c_z;&c_mol;&c_b;&c_je;&c_r;&c_a;&c_g;&c_ch;&c_y;</trd>,
     <trd>&c_u;&c_p;&c_ib;&c_l;&c_mol;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_u;&c_s;&c_mol;&c_c;&c_je;&c_r;&c_a;&c_g;&c_ch;&c_y;</trd>
   </trdgrp>
   <trd lng="de">ersparen</trd>
   <trdgrp lng="en"><trd>to spare</trd>, <trd>save</trd></trdgrp>
   <trdgrp lng="fr">
     <trd>&eacute;pargner<klr> (quelque chose &agrave;
quelqu'un)</klr></trd>
   </trdgrp>
   <trd lng="hu">megk&iacute;m&eacute;l<klr tip="ind"> (vkit
vmit&odblac;l)</klr></trd>
   <trdgrp lng="it">
     <trd>evitare<klr> (qualcosa a qualcuno)</klr></trd>,
     <trd>risparmiare<klr> (far evitare)</klr></trd>
   </trdgrp>
   <trd lng="nl">besparen</trd>
   <trd lng="pl">oszcz&eogonek;dza&cacute; <klr>(komu&sacute;, czego&sacute;)</klr></trd>
   <trd lng="pt">poupar</trd>
   <trd lng="ru">&c_u;&c_b;&c_je;&c_r;&c_je;&c_ch;&c_mol;</trd>
   <trd lng="sv">bespara</trd>
   </snc>
  <trdgrp lng="ja">
    <trd>&#x514D;&#12376;&#x308B;  <pr>&#12417;&#12435;&#12376;&#x308B;</pr></trd>,
    <trd>&#x514D;&#x308C;&#12373;&#x305B;&#x308B; 
    <pr>&#x307E;&#x306C;&#x304C;&#x308C;&#12373;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_u;&c_b;&c_je;&c_r;&c_ib;&c_g;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_je;&c_r;&c_je;&c_g;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="sxpar.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="sxpar.0ujo.KOMUNE">
    <dif>
      Ujo, en kiu oni konservas monerojn, <tld/>a&jcirc;ojn:
      <ekz>
        estis malagrable por mi ka&scirc;iri &scirc;trumpete en la &ccirc;ambron
        de mia patrino, kaj preni mian <tld/>ujon el &scirc;ia
        skribotablo
        <fnt>
        <aut>H. Hesse, trad. D. Karthaus</aut>
        <vrk>Demian</vrk>
        <lok>2007</lok>
        </fnt>.
       </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_a;&c_r;&c_b;&c_o;&c_n;&c_k;&c_a;</trd>
  <trd lng="fr">tirelire</trd>
  <trd lng="pl">skarbonka</trd>
</drv>

<drv mrk="sxpar.mal0i">
   <kap>mal<tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc mrk="sxpar.mal0i.KOMUNE">
     <dif>
       Elspezi multe kaj senprudente:
       <ekz>
         mal<tld/>ulo &gcirc;uas nelonge, avarulo neniam
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         por kio ni vane mal<tld/>as tempon
         <fnt>
           <aut>I. Efremov, trad. J. Finkel</aut>,
           <vrk><url ref="http://www.esperanto.mv.ru/Efremov/horo.html">
           Horo de Bovo</url></vrk>,
           <lok>2005-2011</lok>
         </fnt>.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="glit">elgliti</ref>,
       <ref cel="gren.0o">greno</ref>,
       <ref cel="tru.0o">truo</ref>
     </refgrp>
   </snc>
   <trdgrp lng="be">
     <trd>&c_m;&c_a;&c_r;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_m;&c_a;&c_r;&c_n;&c_a;
     &c_t;&c_r;&c_a;&c_c;&c_ib;&c_c;&c_mol;</trd>,
     <trd>&c_m;&c_a;&c_r;&c_n;&c_a;&c_t;&c_r;&c_a;&c_v;&c_ib;&c_c;&c_mol;</trd>,
     <trd>&c_r;&c_a;&c_s;&c_t;&c_r;&c_a;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_r;&c_a;&c_s;&c_t;&c_r;&c_a;&c_n;&c_zh;&c_y;&c_r;&c_y;&c_c;&c_mol;</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>vergeuden</trd>,
     <trd>verschwenden</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>to waste</trd>,
     <trd>squander</trd>,
     <trd>dissipate</trd>
   </trdgrp>
   <trdgrp lng="es">
     <trd>derrochar</trd>,
     <trd>dilapidar</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>dilapider</trd>,
     <trd>gaspiller</trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>t&eacute;kozol</trd>,
     <trd>pazarol</trd>
   </trdgrp>
   <trdgrp lng="it">
     <trd>sprecare<klr> (specie di forze o energia)</klr></trd>,
     <trd>sperperare</trd>,
     <trd>prodigare<klr> (non risparmiare)</klr></trd>,
     <trd><ind>spendere</ind> e spandere</trd>
   </trdgrp>
   <trdgrp lng="ja">
    <trd>&#x6D6A;&#x8CBB;&#12377;&#x308B;  <pr>&#x308D;&#12358;&#12402;&#12377;&#x308B;</pr></trd>,
    <trd>&#12416;&#12384;&#36963;&#12356;&#12377;&#x308B;  <pr>&#12416;&#12384;&#12389;&#x304B;&#12356;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
     <trd>verkwisten</trd>,
     <trd>verkwanselen</trd>
   </trdgrp>
   <trdgrp lng="pl">
     <trd>trwoni&cacute;</trd>,
     <trd>marnotrawi&cacute;</trd>
   </trdgrp>
   <trdgrp lng="pt">
     <trd>gastar</trd>,
     <trd>desperdi&ccedil;ar</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_t;&c_r;&c_a;&c_n;&c_zh;&c_i;&c_r;&c_i;&c_t;&c_mol;</trd>,
     <trd>&c_t;&c_r;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>,
     <trd>&c_r;&c_a;&c_s;&c_t;&c_o;&c_ch;&c_a;&c_t;&c_mol;</trd>
   </trdgrp>
   <trd lng="sv">sl&ouml;sa</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_r;&c_a;&c_t;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_t;&c_r;&c_a;&c_ch;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43C;&c_a;&c_r;&#x43D;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&#x43E;&c_z;&c_t;&c_r;&c_i;&#x43D;&#x44C;&#x43A;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="sxpar.mal0ema">
  <kap>mal<tld/>ema</kap>
  <snc mrk="sxpar.mal0ema.KOMUNE">
    <dif>
      Kiu facile mal<tld/>as, volonte elspezas, fordonemas:
      <ekz>
        suno super&scirc;utis lin per mal<tld/>ema brilo
        <fnt>
          <aut>T. Mann, trad. P. Buijnsters</aut>,
          <vrk>La morto en Venecio</vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_a;&c_sh;&c_ch;&c_a;&c_d;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_e;&c_k;&c_a;&c_n;&c_o;&c_m;&c_n;&c_y;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_ib;&c_d;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_n;&c_a;&c_t;&c_r;&c_a;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="en">wasteful</trd>
  <trd lng="es">derrochador</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;pensier</trd>,
    <trd>prodigue</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6D6A;&#x8CBB;&#30294;&#x306E;  <pr>&#x308D;&#12358;&#12402;&#x304F;&#x305B;&#x306E;</pr></trd>,
    <trd>&#x6C17;&#x524D;&#x306E;&#12424;&#12356;  <pr>&#x304D;&#x307E;&#12360;&#x306E;&#12356;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>marnotrawny</trd>,
    <trd>rozrzutny</trd>
  </trdgrp>
  <trd lng="uk">&#x43C;&c_a;&c_r;&#x43D;&#x43E;&c_t;&c_r;&c_a;&c_t;&#x43D;&c_i;&c_j;</trd>
</drv>

<drv mrk="sxpar.tempomal0o">
  <kap>tempomal<tld/>o</kap>
  <snc mrk="sxpar.tempomal0o.KOMUNE">
    <dif>
      Mal<tld/>o de tempo, vana penado a&ubreve; atendado:
      <ekz>
        longa tempomal<tld/>a diskuto
        <fnt>
          <aut>I. Fantom</aut>,
          <vrk><url
          ref="http://www.liberafolio.org/Members/iafantomo/oni-petis-mian-opinion-pri-la-londona-esperanto-klubo-jen-gi">
          Oni petis mian opinion pri La Londona Esperanto-Klubo. Jen
          &gcirc;i!</url></vrk>,
          <lok>libera folio, 2015-03-18</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_n;&c_a;&c_v;&c_a;&c_n;&c_n;&c_je; &c_ch;&c_a;&c_s;&c_u;</trd>
  <trd lng="en">waste of time</trd>
  <trd lng="es">p&eacute;rdida de tiempo</trd>
  <trd lng="fr">perte de temps</trd>
  <trd lng="pl">strata <ind>czasu</ind></trd>
</drv>
