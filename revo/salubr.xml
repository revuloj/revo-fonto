<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: salubr.xml,v 1.16 2024/03/24 08:24:36 revo Exp $">
<kap>
  <rad>salubr</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="salubr.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="salubr.0a.MED">
    <dif>
      <ke><ref tip="dif" cel="favor.san0a.MED">Sanfavora</ref></ke>:
      <ekz>
        &ccirc;u vi estas sana nun? en normala stato? &ccirc;u tio,
        kion vi sentas &mdash; estas por vi <tld/>a, bona?
        <fnt>
        <aut>Ivan Turgenev, trad. V. Kudrjavcev k.a.</aut>,
        <vrk><url ref="&LOdE;Rn-turg1.htm">La unua amo</url></vrk>,
        <lok>Jekaterinburg: Sezonoj, 1993</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>aj akvoj de <nom>Busko Zdr&oacute;j</nom> plej efike helpas kaze de
        re&ubreve;matismo
        <fnt>
          <aut>G. Kosiarska</aut>,
          <vrk><url ref="http://www.polskieradio.pl/polonia/article.asp?tId=33141">Busko
          Zdr&oacute;j</url></vrk>,
          <lok>Radio Polonia, 2006-02-18</lok></fnt>;
      </ekz>
      <ekz>
        la ne<tld/>a tempo, kiam perfekta mensa sano estas maltrankviliga signo de
        frenezeco
        <fnt>
          <bib>Monato</bib>
          <aut>Paulo Silas</aut>
          <vrk><url ref="&Monato;/2015/011507.php">La lastaj tagoj de Lillo</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
   </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_r;&c_y;&c_s;&c_n;&c_y; &c_d;&c_l;&c_ja; <ind>&c_z;&c_d;&c_a;&c_r;&c_o;&c_w;&c_ja;</ind></trd>,
    <trd>&c_g;&c_a;&c_ju;&c_ch;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">salubre</trd>
  <trdgrp lng="cs">
    <trd>prosp&ecaron;&scaron;n&yacute; zdrav&iacute;</trd>,
    <trd>zdrav&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gesund<klr>(heitsf&ouml;rderlich)</klr></trd>,
    <trd>heilsam</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>salubrious</trd>,
    <trd>healthy</trd>
  </trdgrp>
  <trd lng="es">saludable</trd>
  <trdgrp lng="fr">
    <trd>bon pour la sant&eacute;</trd>,
    <trd>sain<klr> (bon pour la sant&eacute;)</klr></trd>,
    <trd>salubre</trd>
  </trdgrp>
  <trd lng="it">salubre</trd>
  <trd lng="pl">zdrowotny</trd>
  <trd lng="ru">&c_c;&c_je;&c_l;&c_je;&c_b;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">zdraviu prospe&scaron;n&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_o;&c_r;&c_i;&c_s;&c_n;&c_i;&c_j; &c_d;&c_l;&c_ja; &c_z;&c_d;&c_o;&c_r;&c_o;&c_v;&#8217;&c_ja;</trd>,
    <trd>&c_s;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_l;&c_i;&c_v;&c_i;&c_j; &c_d;&c_l;&c_ja; &c_z;&c_d;&c_o;&c_r;&c_o;&c_v;&#8217;&c_ja;</trd>,
    <trd>&c_c;&c_ib;&c_l;&c_ju;&c_shch;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>

