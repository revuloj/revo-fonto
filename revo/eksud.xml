<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: eksud.xml,v 1.14 2017/10/31 23:11:47 revo Exp $">
<kap>
  <rad>eksud</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="eksud.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="eksud.0i.FIZL">
    <dif>
      Eliri kiel &scirc;vito tra histo,
      <ref tip="vid" cel="sxvit.0igxi">&scirc;viti&gcirc;i</ref>:
      <ekz>
        la sango <tld/>as kelkafoje tra poroj;
      </ekz>
      <ekz>
        &scirc;viteroj sen&ccirc;ese <tld/>is el liaj poroj       
        <fnt>
          <aut>Mo Yan, trad. Vejdo</aut>
          <vrk>Travidebla Ru&gcirc;a Rafano</vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lik.0i">liki</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>exsudovat</trd>,
    <trd>vym&ecaron;&scaron;ovat exsud&aacute;t</trd>,
    <trd>vypocovat se</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>ausschwitzen</trd>,
     <trd>absondern</trd>
  </trdgrp>
  <trd lng="fr">exsuder</trd>
  <trd lng="hu">izzadm&aacute;ny k&eacute;pz&odblac;dik</trd>
  <trd lng="pl">wysi&eogonek;ka&cacute;</trd>
  <trd lng="pt">exsudar</trd>
  <trd lng="sk">vypoti&tcaron;</trd>
</drv>

<drv mrk="eksud.0o">
  <kap><tld/>o</kap>
  <snc mrk="eksud.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Ago de tio, kio <tld/>as.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Ausschwitzen</trd>,
     <trd>Absonderung</trd>
  </trdgrp>
  <trd lng="fr">exsudation</trd>
  <trdgrp lng="hu">
    <trd>izzadm&aacute;nyk&eacute;pz&odblac;d&eacute;s</trd>,
    <trd>exud&aacute;ci&oacute;</trd>
  </trdgrp>
  <trd lng="pl">wysi&eogonek;k</trd>
</drv>

<drv mrk="eksud.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="eksud.0ajxo.FIZL">
    <dif>
      Likva&jcirc;o, kiu <tld/>as.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Exsudat</trd>,
     <trd>Absonderung</trd>
  </trdgrp>
  <trd lng="fr">exsudat</trd>
  <trdgrp lng="hu">
    <trd>izzadm&aacute;ny</trd>,
    <trd>exud&aacute;tum</trd>
  </trdgrp>
  <trd lng="pl">wysi&eogonek;k</trd>
  <trd lng="ru">&c_e;&c_k;&c_s;&c_s;&c_u;&c_d;&c_a;&c_t;</trd>
</drv>
</art>
