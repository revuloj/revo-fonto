<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: eksud.xml,v 1.18 2023/11/12 18:27:36 revo Exp $">
<kap>
  <rad>eksud</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="eksud.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="eksud.0i.FIZL">
    <dif>
      Eliri kiel &scirc;vito tra histo,
      <ref tip="vid" cel="sxvit.0igxi">&scirc;viti&gcirc;i</ref>:
      <ekz>
        la sango <tld/>as kelkafoje tra poroj;
      </ekz>
      <ekz>
        &scirc;viteroj sen&ccirc;ese <tld/>is el liaj poroj
        <fnt>
          <aut>Mo Yan, trad. Vejdo</aut>
          <vrk>Travidebla Ru&gcirc;a Rafano</vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lik.0i">liki</ref>
    </refgrp>
  </snc>
  <snc mrk="eksud.0i.elsxviti">
    <dif>
      El<ref tip="dif" cel="sxvit.0i.KOMUNE">&scirc;viti</ref>:
      <ekz>
        la plantoj <tld/>as tiujn oleojn <klr>[por]</klr> &leftquot;averti&rightquot; &ndash; se tiel diri &ndash; la
        semojn, ke ne estas sufi&ccirc;e da akvo por kreski
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2015/011480.php"
              >Pluveraj veroj</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>exsudovat</trd>,
    <trd>vym&ecaron;&scaron;ovat exsud&aacute;t</trd>,
    <trd>vypocovat se</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>ausschwitzen</trd>,
     <trd>absondern</trd>
  </trdgrp>
  <trd lng="fr">exsuder</trd>
  <trd lng="hu">izzadm&aacute;ny k&eacute;pz&odblac;dik</trd>
  <trdgrp lng="ja">
    <trd>&#12395;&#12376;&#12415;&#20986;&#12427;  <pr>&#12395;&#12376;&#12415;&#12391;&#12427;</pr></trd>,
    <trd>&#12375;&#12415;&#20986;&#12427;  <pr>&#12375;&#12415;&#12391;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="pl">wysi&eogonek;ka&cacute;</trd>
  <trd lng="pt">exsudar</trd>
  <trd lng="sk">vypoti&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_o;&c_s;&c_o;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja; &c_n;&c_a;&c_z;&c_o;&c_v;&c_n;&c_ib;</trd>,
    <trd>&c_v;&c_i;&c_s;&c_t;&c_u;&c_p;&c_a;&c_t;&c_i; (&c_p;&c_r;&c_o; &c_k;&c_r;&c_o;&c_v; &c_ib; &c_t;.&c_p;.)</trd>
  </trdgrp>
</drv>

<drv mrk="eksud.0o">
  <kap><tld/>o</kap>
  <snc mrk="eksud.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Ago de tio, kio <tld/>as.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Ausschwitzen</trd>,
     <trd>Absonderung</trd>
  </trdgrp>
  <trd lng="fr">exsudation</trd>
  <trdgrp lng="hu">
    <trd>izzadm&aacute;nyk&eacute;pz&odblac;d&eacute;s</trd>,
    <trd>exud&aacute;ci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#28402;&#20986;  <pr>&#12375;&#12435;&#12375;&#12421;&#12388;</pr></trd>,
    <trd>&#28024;&#20986;  <pr>&#12375;&#12435;&#12375;&#12421;&#12388;</pr></trd>
  </trdgrp>
  <trd lng="pl">wysi&eogonek;k</trd>
  <trdgrp lng="uk">
    <trd>&c_je;&c_k;&c_s;&c_u;&c_d;&c_a;&c_c;&c_ib;&c_ja;</trd>,
    <trd>&c_v;&c_i;&c_p;&c_o;&c_t;&c_ib;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="eksud.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="eksud.0ajxo.FIZL">
    <dif>
      Likva&jcirc;o, kiu <tld/>as.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Exsudat</trd>,
     <trd>Absonderung</trd>
  </trdgrp>
  <trd lng="fr">exsudat</trd>
  <trdgrp lng="hu">
    <trd>izzadm&aacute;ny</trd>,
    <trd>exud&aacute;tum</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#28402;&#20986;&#28082;  <pr>&#12375;&#12435;&#12375;&#12421;&#12388;&#12360;&#12365;</pr></trd>,
    <trd>&#28024;&#20986;&#28082;  <pr>&#12402;&#12383;&#12391;&#12360;&#12365;</pr></trd>
  </trdgrp>
  <trd lng="pl">wysi&eogonek;k</trd>
  <trd lng="ru">&c_e;&c_k;&c_s;&c_s;&c_u;&c_d;&c_a;&c_t;</trd>
  <trdgrp lng="uk">
    <trd>&c_je;&c_k;&c_s;&c_u;&c_d;&c_a;&c_t;</trd>,
    <trd>&c_v;&c_i;&c_p;&c_ib;&c_t;</trd>
  </trdgrp>
</drv>
</art>
