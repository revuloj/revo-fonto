<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>eksud</rad>/i
</kap>
<drv mrk="eksud.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Eliri kiel &scirc;vito tra histo:
      <ekz>
        la sango <tld/>as kelkafoje tra poroj.
      </ekz>
    </dif>
    <ref tip="vid" cel="sxvit.0igxi">&scirc;viti&gcirc;i.</ref>
  </snc>
  <trd lng="fr">exsuder</trd>
</drv>

<drv mrk="eksud.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago de tio, kio <tld/>as.
    </dif>
  </snc>
  <trd lng="fr">exsudation</trd>
</drv>

<drv mrk="eksud.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Likva&jcirc;o, kiu <tld/>as.
    </dif>
  </snc>
  <trd lng="fr">exsudat</trd>
  <trd lng="ru">&c_e;&c_k;&c_s;&c_s;&c_u;&c_d;&c_a;&c_t;</trd>
</drv>
</art>
