<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>eksud</rad>/i
</kap>
<drv mrk="eksud.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Eliri kiel &scirc;vito tra histo:
      <ekz>
        la sango <tld/>as kelkafoje tra poroj.
      </ekz>
    </dif>
    <ref tip="vid" cel="sxvit.0igxi">&scirc;viti&gcirc;i.</ref>
  </snc>
  <trdgrp lng="de">
     <trd>ausschwitzen</trd>,
     <trd>absondern</trd>
  </trdgrp>
  <trd lng="fr">exsuder</trd>
  <trd lng="hu">izzadm&aacute;ny k&eacute;pz&odblac;dik</trd>
  <trd lng="pt">exsudar</trd>
</drv>

<drv mrk="eksud.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago de tio, kio <tld/>as.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Ausschwitzen</trd>,
     <trd>Absonderung</trd>
  </trdgrp>
  <trd lng="fr">exsudation</trd>
  <trdgrp lng="hu">
    <trd>izzadm&aacute;nyk&eacute;pz&odblac;d&eacute;s</trd>,
    <trd>exud&aacute;ci&oacute;</trd>
  </trdgrp>
</drv>

<drv mrk="eksud.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Likva&jcirc;o, kiu <tld/>as.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Exsudat</trd>,
     <trd>Absonderung</trd>
  </trdgrp>
  <trd lng="fr">exsudat</trd>
  <trdgrp lng="hu">
    <trd>izzadm&aacute;ny</trd>,
    <trd>exud&aacute;tum</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_k;&c_s;&c_s;&c_u;&c_d;&c_a;&c_t;</trd>
</drv>
</art>
