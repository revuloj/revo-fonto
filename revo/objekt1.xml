<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>objektiv</rad>/a
</kap>
<drv mrk="objekt1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Efektive ekzistanta ekster la penso:
      <ekz>
        Kant neas la <tld/>an realecon de
        niaj ideoj.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Bazita sur senpartia esploro, sendependa de individuaj pla&ccirc;oj:
      <ekz>
        tre malmultaj homoj kapablas observi kaj raporti <tld/>e.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">objektiv</trd>
  <trd lng="nl">objectief</trd>
  <trdgrp lng="hu">
    <trd>objekt&iacute;v</trd>,
    <trd>t&aacute;rgyilagos</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_b;&c_malmol;&c_je;&c_k;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="objekt1.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Karaktero de io <tld/>a:
      <ekz>
        la <tld/>eco de niaj konoj;
      </ekz>
      <ekz>
        la a&ubreve;toro ne deflanki&gcirc;is de la plej severa
        <tld/>eco en la prezento de la demando;
      </ekz>
      <ekz>
        kutime forestas <tld/>eco en &ccirc;iu ajn agitado.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Objektivit&auml;t</trd>
  <trdgrp lng="hu">
    <trd>t&aacute;rgyilagoss&aacute;g</trd>,
    <trd>objektivit&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">objectiviteit</trd>
  <trd lng="ru">&c_o;&c_b;&c_malmol;&c_je;&c_k;&c_t;&c_i;&c_v;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>
</art>
