<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ginekologi</rad>/o
</kap>
<drv mrk="gineko1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Parto de la medicino, kiu pritraktas la seksajn organojn de la virino,
      ilian fiziologion k malsanojn.  
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_n;&c_e;&c_k;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Frauenheilkunde</trd>,
    <trd>Gyn&auml;kologie</trd>
  </trdgrp>
  <trd lng="es">ginecolog&iacute;a</trd>
  <trdgrp lng="hu">
    <trd>n&odblac;gy&oacute;gy&aacute;szat</trd>,
    <trd>ginekol&oacute;gia</trd>
  </trdgrp>
  <trd lng="nl">gynecologie</trd>
  <trd lng="pt">ginecologia</trd>
  <trd lng="ru">&c_g;&c_i;&c_n;&c_je;&c_k;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trdgrp lng="tr">
    <trd>nisaiye</trd>,
    <trd>jinekoloji</trd>
  </trdgrp>
</drv>
<drv mrk="gineko1.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Scienculo studanta <tld/>o; specialisto en <tld/>o.  
    </dif>
    <ref tip="sin" cel="gineko.0o">ginekologo.</ref>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_n;&c_e;&c_k;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trdgrp lng="de">
    <trd>Frauenarzt</trd>,
    <trd>Gyn&auml;kologe</trd>
  </trdgrp>
  <trd lng="fr">gyn&eacute;cologue</trd>
  <trd lng="es">ginec&oacute;logo</trd>
  <trdgrp lng="hu">
    <trd>n&odblac;gy&oacute;gy&aacute;sz</trd>,
    <trd>ginekol&oacute;gus</trd>
  </trdgrp>
  <trd lng="nl">gynecoloog</trd>
  <trdgrp lng="pt">
   <trd>ginec&oacute;logo</trd>, 
   <trd>ginecologista</trd> 
  </trdgrp>
  <trd lng="ru">&c_g;&c_i;&c_n;&c_je;&c_k;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="tr">jinekolog</trd></drv>
  </art>
