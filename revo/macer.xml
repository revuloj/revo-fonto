<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: macer.xml,v 1.20 2023/10/19 15:13:16 revo Exp $">
<kap>
  <rad>macer</rad>/i
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="macer.0i">
  <kap>
  <tld/>i
  </kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">KUI</uzo>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Meti korpon en malvarman likva&jcirc;on por <ref cel="solv.0i">solvi</ref> &gcirc;iajn solveblajn
      substancojn:
      <ekz>
        <tld/>i planton en alkoholo, fruktojn en vinagro;
      </ekz>
      <ekz>
        <tld/>i tekseblan planton en likva&jcirc;o por apartigi &gcirc;iajn fibrojn
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470; 12</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="super">
      <ref cel="ekstra.0i">ekstrakti</ref>,
      <ref cel="tremp.0i">trempi</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="infuz.0i">infuzi</ref>,
      <ref cel="lesiv.0i">lesivi</ref>,
      <ref cel="marin.0i">marini</ref>,
      <ref cel="satur.0i">saturi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_m;&c_o;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_m;&c_o;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">macerar</trd>
  <trdgrp lng="cs">
     <trd>macerovat</trd>,
     <trd>m&aacute;&ccaron;et</trd>,
     <trd>vyluhovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>auslaugen</trd>,
    <trd>ausziehen</trd>,
    <trd>einlegen</trd>,
    <trd>mazerieren</trd>
  </trdgrp>
  <trd lng="en">macerate</trd>
  <trd lng="es">macerar</trd>
  <trd lng="fr">mac&eacute;rer</trd>
  <trdgrp lng="hu">
    <trd>&aacute;ztat</trd>,
    <trd>macer&aacute;l<klr tip="ind"> (&aacute;ztat)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x51B7;&#x6D78;&#12377;&#x308B;  <pr>&#12402;&#12420;&#12402;&#x305F;&#12377;&#x308B;</pr></trd>,
    <trd>&#x6D78;&#x6F2C;&#12377;&#x308B; <pr>&#12375;&#12435;&#12379;&#12365;&#12377;&#12427;</pr></trd>,
    <trd>&#x82E6;&#12375;&#12417;&#x308B;  <pr>&#x304F;&#x308B;&#12375;&#12417;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>macereren</trd>,
    <trd>inweken</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>macerowa&cacute;</trd>,
    <trd>rozmi&eogonek;kcza&cacute;</trd>
  </trdgrp>
  <trd lng="pt">macerar</trd>
  <trdgrp lng="ro">
    <trd>macera</trd>,
    <trd>muia</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_m;&c_a;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_c;&c_je;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_m;&c_o;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_o;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_m;&c_a;&c_c;&c_je;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_m;&c_o;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_u;&c_m;&c_je;&c_r;&c_t;&c_v;&c_l;&c_ja;&c_t;&c_i; &c_p;&c_l;&c_o;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
