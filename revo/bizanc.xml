<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Bizanc</rad>/o
</kap>
<drv mrk="bizanc.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      Antikva nomo de
      <ref tip="sin" cel="istanbul.0o">Istanbulo</ref>,
      &gcirc;is la jaro 324a, post kiu Konstanteno la Granda alinomis
      &gcirc;in
      <ref tip="sin" cel="konstantinopol.0o">Konstantinopolo</ref>:
      <ekz>
        &gcirc;i estis inventita proksimume en la 9a jarcento en
        <tld/>o, de kie
        forko ekkonkeris la mondon<fnt><aut>Araneo</aut>:
        <vrk>Forko</vrk>, <bib>Monato</bib><lok>jaro 2002a, numero 5a,
        p.
        24a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_B;&c_ib;&c_z;&c_a;&c_n;&c_t;&c_y;&c_j;</trd>
  <trd lng="bg">&c_V;&c_i;&c_z;&c_a;&c_n;&c_t;&c_i;&c_ja;</trd>
  <trd lng="de">Byzanz</trd>
  <trd lng="en">Byzantium</trd>
  <trdgrp lng="fr">
    <trd>byzance</trd>
  </trdgrp>
  <trd
  lng="grc">&Beta;&ypsilon;&zeta;&alfa_ton;&ny;&tau;&jota;&omikron;&ny;</trd>
  <trd lng="hu">Biz&aacute;nc</trd>
  <trd lng="pl">Bizancjum</trd>
  <trd lng="ru">&c_V;&c_i;&c_z;&c_a;&c_n;&c_t;&c_i;&c_j;</trd>
</drv>
</art>
