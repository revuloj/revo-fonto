<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Bizanc</rad>/o
    </kap>
    <drv mrk="bizanc.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">HIS</uzo>
      <snc>
	<dif>
	  Antikva nomo de
	  <ref tip="sin" cel="instanbul.0o">Istanbulo</ref>,
	  &gcirc;is la jaro 324a, post kiu Konstanteno la Granda alinomis &gcirc;in
	  <ref tip="sin" cel="konstantinopol.0o">Konstantinopolo</ref>.
	</dif>
      </snc>
      <trd lng="de">Byzanz</trd>
      <trd lng="en">Byzantium</trd>
      <trd lng="grc">&Beta;&ypsilon;&zeta;&alfa_ton;&ny;&tau;&jota;&omikron;&ny;</trd>
      <trd lng="ru">&c_V;&c_i;&c_z;&c_a;&c_n;&c_t;&c_i;&c_j;</trd>
    </drv>

  </art>

