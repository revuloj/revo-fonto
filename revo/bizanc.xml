<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bizanc.xml,v 1.19 2023/10/29 19:11:02 revo Exp $">
<kap>
  <rad>Bizanc</rad>/o, 
  <var><kap><rad var="ti">Bizanti</rad>/o <fnt><bib>Fab3</bib></fnt></kap></var>
</kap>
<drv mrk="bizanc.0o">
  <kap><tld/>o, <var><kap><tld var="ti"/>o</kap></var></kap>
  <uzo tip="fak">HIS</uzo>
  <snc mrk="bizanc.0o.antikva">
    <dif>
      Antikva nomo de
      <ref tip="sin" cel="istanbul.0o">Istanbulo</ref>,
      &gcirc;is la jaro 324a, post kiu Konstanteno la Granda alinomis
      &gcirc;in <ref tip="sin" cel="konstantinopol.0o">Konstantinopolo</ref>:
      <ekz>
        oni veturas al Bosforo, por tie halti por kelke da horoj
        kaj rigardi la lokon, kie staris <tld var="ti"/>o
        <fnt><bib>Fab3</bib>, <lok>Post jarmiloj</lok></fnt>;
      </ekz>
      <ekz>
        grupo <klr>[de cignoj]</klr> kun brilantaj plumoj kaj fidelecaj okuloj eklo&gcirc;is
        en <tld var="ti"/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Cigna nesto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i estis inventita proksimume en la 9a jarcento en
        <tld/>o, de kie forko ekkonkeris la mondon
        <fnt>
          <bib>Monato</bib>
          <aut>Araneo</aut>:
          <vrk>Forko</vrk>,
          <lok>jaro 2002a, numero 5a, p. 24a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="b"/>a imperiestro
        <fnt>
          <bib>Monato</bib>
          <aut>Dimitar Ha&gcirc;iev</aut>
          <vrk><url ref="&Monato;/2006/008957.php">Religioj en Bulgario</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x62DC;&#x4F54;&#x5EAD; <pr>b&#xE0;izh&#xE0;nt&#xED;ng</pr></trd>,
    <trd>&#x62DC;&#x5360;&#x5EAD; <pr>b&#xE0;izh&#xE0;nt&#xED;ng</pr></trd>
  </trdgrp>
</snc>
  <trd lng="grc">
&Beta;&ypsilon;&zeta;&alfa_ton;&ny;&tau;&jota;&omikron;&ny;</trd>
  <trd lng="he">&#x5D1;&#x5D9;&#x5D6;&#x5E0;&#x5D8;&#x5D9;&#x5D5;&#x5DF;</trd>
  <trd lng="ja">&#x30D3;&#x30B6;&#x30F3;&#x30C1;&#x30A6;&#x30E0;</trd>
  <trd lng="lat">Byzantium</trd>
  <trd lng="be">&c_B;&c_ib;&c_z;&c_a;&c_n;&c_t;&c_y;&c_j;</trd>
  <trd lng="bg">&c_V;&c_i;&c_z;&c_a;&c_n;&c_t;&c_i;&c_ja;</trd>
  <trd lng="ca">Bizanci</trd>
  <trdgrp lng="cs">
    <trd>Byzancie</trd>,
    <trd>Byzantsk&aacute; &rcaron;&iacute;&scaron;e</trd>
  </trdgrp>
  <trd lng="de">Byzanz</trd>
  <trd lng="en">Byzantium</trd>
  <trdgrp lng="fr">
    <trd>byzance</trd>
  </trdgrp>
  <trd lng="hu">Biz&aacute;nc</trd>
  <trd lng="it">Bisanzio</trd>
  <trd lng="pl">Bizancjum</trd>
  <trd lng="ru">&c_V;&c_i;&c_z;&c_a;&c_n;&c_t;&c_i;&c_j;</trd>
  <trd lng="sk">Byzantsk&aacute; r&iacute;&scaron;a</trd>
  <trd lng="tr">Bizans</trd>
  <trd lng="uk">&#x412;&#x456;&#x437;&#x430;&#x43D;&#x442;&#x456;&#x439;</trd>
</drv>
</art>
