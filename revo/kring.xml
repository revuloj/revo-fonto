<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kring</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kring.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kring.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Malgranda ringoforma seka sukerita kuko:
      <ekz>
        krustetaj <tld/>oj, <tld/>etoj en formoj de &ccirc;iuj dombestoj, 
        katoj, hundoj, bovoj, &ccirc;evaloj, azenoj, &scirc;afoj kaj e&ccirc; kunikloj
        <fnt><bib>KielAkvo</bib>, <lok>1:3</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_a;&c_r;&c_a;&c_n;&c_a;&c_k;</trd>
  <trd lng="hu">perec (&eacute;des)</trd>
  <trd lng="de">Kringel</trd>
  <trd lng="fr">craquelin</trd>
  <trd lng="pl">obwarzanek</trd>
  <trdgrp lng="pt">
    <trd>rosquinha</trd>,
    <trd>rosquilha</trd>,
    <trd>sequilho</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_je;&c_n;&c_d;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_b;&c_a;&c_r;&c_a;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
