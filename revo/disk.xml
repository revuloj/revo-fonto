<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>disk</rad>/o
</kap>
<drv mrk="disk.0o">
  <kap><tld/>o</kap>
  <snc mrk="disk.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Metala ronda plata&jcirc;o, kiun la antikvaj grekoj
      uzis en la &jcirc;etludoj:
      <ekz>
        Diskobolo estas statuo de <tld/>o&jcirc;etanto.
      </ekz>
    </dif>
    <trd lng="de">Diskus</trd>
    <trd lng="nl">discus</trd>
  </snc>
  <snc mrk="disk.0o.MUZ.KOMP">
    <uzo tip="fak">MUZ</uzo>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <tld lit="D"/>forma objekto, sur kiu estas registritaj
      iuj datumoj:
      <ekz>
        son<tld/>o;
      </ekz>
      <ekz>
        magneta <tld/>o, <tld/>eto;
      </ekz>
      <ekz>
        lum<tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Platte</trd>,
      <trd>Diskette</trd>,
      <trd>CD</trd> ktp.
    </trdgrp>
    <trdgrp lng="nl">
      <trd>schijf</trd>,
      <trd>diskette</trd>,
      <trd>CD</trd>,
      <trd>plaat <klr tip="ind">(grammofoon-)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="disk.0o.diversa">
    <dif>
      Objekto <tld/>on similanta:
      <ekz>
        <tld/>o de suno, de fervoja signalo;
      </ekz>
      <ekz>
        damluda <ind><tld/>o</ind>.
        <trd lng="ru">&c_sh;&c_a;&c_sh;&c_k;&c_a; <klr>(&c_v; &c_sh;&c_a;&c_sh;&c_k;&c_a;&c_h;)</klr></trd>
      </ekz>
    </dif>
    <trd lng="nl">schijf</trd>
  </snc>
  <snc mrk="disk.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="disk.0o.cirklo">
      <fnt><bib>MatVort</bib></fnt>
      <ref tip="dif" cel="cirkl.0o.surfaco">cirkla surfaco.</ref>
    </subsnc>
    <subsnc mrk="disk.0o.en_kompleksa_ebeno">
      <dif>
        <klr>(en la aro de <ref cel="komple.0o.MAT">kompleksoj</ref>)</klr>
      </dif>
      <ref tip="dif" cel="glob.0o.metrikaspaco">globo.</ref>
    </subsnc>
    <adm>
      Kontroli, ke la jamaj tradukoj plu validas.
      <aut>MB</aut>
    </adm>
  </snc>
  <trd lng="de">Scheibe</trd>
  <trd lng="en">disk</trd>
  <trd lng="fr">disque</trd>
  <trd lng="ru">&c_d;&c_i;&c_s;&c_k;</trd>
</drv>

<drv mrk="disk.0o.magneta">
  <kap>magneta <tld/>o</kap>
  <uzo tip="fak">KOMP</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="datum.0portilo">Datumportilo</ref>
      en formo de <tld/>o tegita per magnetebla tavolo por
      registrado kaj legado de datumoj:
      <ekz>
        skribi, registri datumojn sur magnetan <tld/>on;
      </ekz>
      <ekz>
        legi, vi&scirc;i datumojn de magneta <tld/>o.
      </ekz>
    </dif>
    <ref tip="sub" cel="disk.0eto">disketo</ref>
    <ref tip="vid" cel="disk.0o.fiksita">fiksita disko</ref>
  </snc>
  <trd lng="en">magnetic disk</trd>
  <trd lng="fr">disque magn&eacute;tique</trd>
  <trd lng="ru">&c_m;&c_a;&c_g;&c_n;&c_i;&c_t;&c_n;&c_y;&c_j; &c_d;&c_i;&c_s;&c_k;</trd>
</drv>

<drv mrk="disk.0eto">
  <kap><tld/>eto</kap>
  <uzo tip="fak">KOMP</uzo>
  <snc>
    <dif>
      Relative malgranda demetebla
      <ref cel="disk.0o.magneta">magneta <tld/>o</ref>
      uzata por datumtransportado a&ubreve; konservado de malofte uzataj
      datumaroj:
      <ekz>
        tiel nomatajn <ctl>3&#xbd;-colajn <tld/>etojn</ctl> la normo
        fakte difinas kiel 9-centimetrajn <tld/>etojn.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>floppy disk</trd>, <trd>diskette</trd>
  </trdgrp>
  <trd lng="nl">diskette</trd>
  <trd lng="fr">disquette</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_i;&c_s;&c_k;&c_je;&c_t;&c_a;</trd>,
    <trd>&c_g;&c_i;&c_b;&c_k;&c_i;&c_j; &c_d;&c_i;&c_s;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="disk.0ingo">
  <kap><tld/>ingo</kap>
  <uzo tip="fak">KOMP</uzo>
  <snc>
    <dif>
      Fendo en diskturnilo, tra kiu la uzanto instalas
      disketon (a&ubreve;, simile, lumdiskon).
    </dif>
  </snc>
  <trd lng="en">disk drive</trd>
  <trd lng="ru">&c_shch;&c_je;&c_l;&c_mol; <ind>&c_d;&c_i;&c_s;&c_k;&c_o;&c_v;&c_o;&c_d;&c_a;</ind></trd>
</drv>

<drv mrk="disk.0turnilo">
  <kap><tld/>turnilo</kap>
  <uzo tip="fak">KOMP</uzo>
  <snc>
    <dif>
      Organo turnanta disketon kaj stiranta la kapetojn por
      skribi datumojn sur la diskon a&ubreve; legi datumojn de sur
      la disketo.
    </dif>
  </snc>
  <trd lng="en">disk unit</trd>
  <trd lng="fr">lecteur</trd>
  <trd lng="ru">&c_d;&c_i;&c_s;&c_k;&c_o;&c_v;&c_o;&c_d;</trd>
</drv>

<drv mrk="disk.0aparato">
  <kap><tld/>aparato</kap>
  <uzo tip="fak">KOMP</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="datum.0portilo">Datumportilo</ref>
      konsistanta el <ref cel="disk.0o.magneta">magneta disko</ref>
      (a&ubreve; pluraj tiaj diskoj samaksaj), la lega-skribaj kapetoj
      kaj ceteraj partoj me&hcirc;anikaj kaj elektronikaj,
      kune muntitaj en metala skatolo.
    </dif>
    <rim>
      Normale komputilo havas diskaparaton kiel sian
      <ref tip="sub" cel="disk.0o.fiksita">fiksitan diskon</ref>;
      sed ekzistas anka&ubreve; demeteblaj diskaparatoj.
    </rim>
  </snc>
  <trd lng="en">hard disk</trd>
  <trd lng="fr">disque dur</trd>
  <trd lng="ru">&c_zh;&c_jo;&c_s;&c_t;&c_k;&c_i;&c_j; &c_d;&c_i;&c_s;&c_k;</trd>
</drv>

<drv mrk="disk.0o.fiksita">
  <kap>fiksita <tld/>o</kap>
  <uzo tip="fak">KOMP</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="disk.0aparato">Diskaparato</ref>
      muntita en komputilon (kontraste al la demeteblaj
      disketoj kaj diskaparatoj).
    </dif>
  </snc>
  <trd lng="de">Festplatte</trd>
  <trdgrp lng="en">
    <trd>fixed disk</trd>, <trd>hard disk</trd>
  </trdgrp>
  <trd lng="nl">harde <ind>schijf</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_i;&c_n;&c_ch;&c_je;&c_s;&c_t;&c_je;&c_r;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_t;&c_o;&c_ja;&c_n;&c_n;&c_y;&c_j; &c_d;&c_i;&c_s;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="disk.lum0o">
  <kap>lum<tld/>o</kap>
  <uzo tip="fak">KOMP</uzo>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <tld lit="D"/>forma
      <ref tip="super" cel="datum.0portilo">datumportilo</ref>,
      funkcianta per lasera registrado kaj legado de datumoj.
      La&ubreve; la normo ISO-9660 lumdiskoj havas la diametron 12cm.
    </dif>
    <rim num="1">
      La terminon <ctl>lumdisko</ctl>, imitantan la koncernan terminon
      &ccirc;inan (guang1pan2), proponis Lee Sau Dan. Simile, en la Islanda
      oni nomas &gcirc;in <ctl>geisladiskur</ctl> (<ctl>radi-disko</ctl>,
      t.e. <ctl>laserradia disko</ctl>).
    </rim>
    <rim num="2">
      Plej ofte, la kunteksto sufi&ccirc;as por kompreni, &ccirc;u
      <ctl>lumdisko</ctl> estas nurlegebla komputila lumdisko
      a&ubreve; sondisko.
    </rim>
  </snc>
  <trdgrp lng="en">
    <trd>compact disc</trd>, <trd>optical disc</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>disque compact</trd>, <trd>disque optique</trd>
  </trdgrp>
  <trd lng="nl">CD</trd>
  <trd lng="pl">p&lstroke;yta CD</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_p;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; &c_d;&c_i;&c_s;&c_k;</trd>,
    <trd>&c_k;&c_o;&c_m;&c_p;&c_a;&c_k;&c_t;-&c_d;&c_i;&c_s;&c_k;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_s;&c_t;&c_i;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="disk.lum0o.ego">
  <kap>lum<tld/>ego</kap>
  <uzo tip="fak">KOMP</uzo>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Grandkapacita speco de <ref cel="disk.lum0o">lumdisko</ref>,
      aperinta en la jaro 1996a, kiu povas enteni 26oble pli da
      datumoj (interalie, filmojn, muzikon, programaron).
    </dif>
  </snc>
  <trd lng="en">DVD</trd>
</drv>

<drv mrk="disk.0o.kompakta">
  <kap>kompakta <tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="disk.lum0o">Lum<tld/>o</ref>.
    </dif>
  </snc>
</drv>

<drv mrk="disk.sub0o">
  <kap>sub<tld/>o</kap>
  <uzo tip="fak">KOMP</uzo>
  <snc>
    <dif>
      &Ccirc;iu el la membroj de
      <ref cel="part.dis0gi.KOMP">dispartigita</ref> <tld/>o
      (precipe <ref cel="disk.0o.fiksita">fiksita <tld/>o</ref>),
      &ccirc;iu kun sia aparta
      <ref tip="prt" cel="dosier.0sistemo">dosiersistemo</ref>:
      <ekz>
        tiu programo kreos du dosierojn &ccirc;e
        via Vindoza sub<tld/>o por Linukso
        <fnt>
        <lok><url
        ref="http://www.task.gda.pl/pub/linux/Mandrake/current/i586/doc/install/eo/install.htm"
        >Instalada Instrukcio &dash; Linuks-Mandrejko 7.2</url></lok>.
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">a <ind>partition</ind> <klr>(of a disk)</klr></trd>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_d;&c_je;&c_l; <klr>(&c_d;&c_i;&c_s;&c_k;&c_a;)</klr></trd>
</drv>

<drv mrk="disk.sfer0o">
  <kap>sfer<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <ref tip="dif" cel="tavol.glob0o">globtavolo.</ref>
  </snc>
  <trd lng="de">Kugelschicht</trd>
  <trd lng="en">spherical <ind>layer</ind></trd>
  <trd lng="fr">segment sph&eacute;rique &agrave; deux bases</trd>
  <trd lng="ru">&c_sh;&c_a;&c_r;&c_o;&c_v;&c_o;&c_j; <ind>&c_s;&c_l;&c_o;&c_j;</ind></trd>
</drv>

</art>
