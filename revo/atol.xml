<art mrk="$Id: atol.xml,v 1.27 2023/10/13 17:08:38 revo Exp $">
<kap>
  <rad>atol</rad>/o
</kap>
<drv mrk="atol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Ringoforma <ref tip="super" cel="insul.0o">insulo</ref>
      formita de koraloj:
      <ekz>
        Usono eksperimentis per hidrogena bombo &ccirc;e la <tld/>o <nom>Bikini</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="&Monato;/2010/010025.php">Bruo pribrua</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la Francfregataj Sablejoj <klr>[&#x2026;]</klr> estas la plej granda <tld/>o de la
        Nordokcidentaj havajaj insuloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=423573">Francfregataj Sablejoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_t;&c_o;&c_l;</trd>
  <trd lng="bg">&c_a;&c_t;&c_o;&c_l;</trd>
  <trd lng="ca">atol</trd>
  <trdgrp lng="cs">
    <trd>atol</trd>,
    <trd>kor&aacute;lov&yacute; ostrov</trd>
  </trdgrp>
  <trd lng="de">Atoll</trd>
  <trd lng="en">atoll</trd>
  <trd lng="es">atol&oacute;n</trd>
  <trd lng="fr">atoll</trd>
  <trd lng="he">&#x5E9;&#x5D5;&#x5E0;&#x5D9;&#x5EA; &#x5D0;&#x5DC;&#x5DE;&#x5D5;&#x5D2;&#x5D9;&#x5DD;</trd>
  <trdgrp lng="hu">
    <trd>atoll</trd>,
    <trd>korallz&aacute;tony</trd>
  </trdgrp>
  <trd lng="it">atollo</trd>
  <trd lng="ja">&#x74B0;&#x7901;  <pr>&#x304B;&#x3093;&#x3057;&#x3087;&#x3046;</pr></trd>
  <trd lng="nl">atol</trd>
  <trd lng="pl">atol</trd>
  <trd lng="ru">&c_a;&c_t;&c_o;&c_l;&c_l;</trd>
  <trdgrp lng="sk">
    <trd>atol</trd>,
    <trd>koralov&yacute; ostrov</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>atoll</trd>,
    <trd>korall&ouml;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x430;&#x442;&#x43E;&#x43B;</trd>,
    <trd>&#x43A;&#x43E;&#x440;&#x430;&#x43B;&#x43E;&#x432;&#x438;&#x439;&#xA0;&#x43E;&#x441;&#x442;&#x440;&#x456;&#x432;</trd>
  </trdgrp>
</drv>
