<kap>
  <ofc>*</ofc>
  <rad>ekzerc</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ekzerc.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ekzerc.0i.PED">
    <dif>
      Lertigi per la praktiko:
      <ekz>
        <tld/>i la soldatojn
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 22</lok></fnt>;
      </ekz>
      <ekz>
        <ind>sin <tld/>i</ind> en parolado<fnt>Z</fnt>;
        <trd lng="be">&c_p;&c_r;&c_a;&c_k;&c_t;&c_y;&c_k;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
        <trd lng="ca">exercitar-se</trd>
        <trdgrp lng="es">
          <trd>entrenarse</trd>,
          <trd>ejercitarse</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>s'<ind>exercer</ind><klr tip="ind">
          (s'entra&icirc;ner)</klr></trd>,
          <trd>s'<ind>entra&icirc;ner</ind><klr tip="ind">
          (s'exercer)</klr></trd>
        </trdgrp>
        <trdgrp lng="pl">
          <trd>&cacute;wiczy&cacute; si&eogonek;</trd>,
          <trd>wprawia&cacute; si&eogonek;</trd>
        </trdgrp>
        <trd lng="ru">&c_u;&c_p;&c_r;&c_a;&c_zh;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
      </ekz>
      <ekz>
        <tld/>i sian orelon<fnt>Z</fnt>,
        sian langon, muskolojn, fingrojn;
      </ekz>
      <ekz>
        <tld/>i sian spiriton per la rezonado, sian memoron;
      </ekz>
      <ekz>
        vere <tld/>ita aktoro;
      </ekz>
      <ekz>
        niaj kongresoj estas <tld/>anta kaj edukanta
        anta&ubreve;parolo <klr>(enkonduko)</klr> por la estonta
        historio de la homaro<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dres.0i">dresi</ref>,
      <ref cel="hard.0i">hardi</ref>,
      <ref cel="prakti.0i">praktiki</ref>
    </refgrp>
    <rim>
      La esprimoj
      <ekz>
        <tld/>i lingvon, <tld/>i ian lernota&jcirc;on
      </ekz>
      a&ubreve; similaj estas mal&gcirc;ustaj &ccirc;ar la verbo
      <tld/>i, normale havas kiel rektan
      komplementon nur ian lernantan personon a&ubreve;
      plilerti&gcirc;ontan
      organon a&ubreve; beston.
    </rim>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_k;&c_t;&c_y;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">exercitar</trd>
  <trdgrp lng="cs">
    <trd>cvi&ccaron;it</trd>,
    <trd>procvi&ccaron;it</trd>,
    <trd>procvi&ccaron;ovat</trd>,
    <trd>vycvi&ccaron;it</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>&uuml;ben</trd>,
    <trd>exerzieren <klr>(soldatojn)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>exercise</trd>,
    <trd>train</trd>,
    <trd>drill</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>ejercitar</trd>,
    <trd>entrenar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>exercer<klr tip="ind"> (entra&icirc;ner)</klr></trd>,
    <trd>entra&icirc;ner<klr tip="ind"> (exercer)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&cacute;wiczy&cacute;</trd>,
    <trd>szkoli&cacute;</trd>,
    <trd>trenowa&cacute;</trd>,
    <trd>wprawia&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>exercitar</trd>,
    <trd>exercer</trd>,
    <trd>treinar</trd>,
    <trd>ensaiar</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_p;&c_r;&c_a;&c_zh;&c_n;&c_ja;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>cvi&ccaron;i&tcaron;</trd>,
    <trd>nacvi&ccaron;ova&tcaron;</trd>
  </trdgrp>
</drv>

