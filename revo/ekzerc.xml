  <trd lng="he">&lamed;&he;&tav;&alef;&mem;&fnun;</trd>
  <trd lng="ja">&#x7DF4;&#x7FD2;&#x3059;&#x308B;  <pr>&#x308C;&#x3093;&#x3057;&#x3085;&#x3046;&#x3059;&#x308B;</pr></trd>
