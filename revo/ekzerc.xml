<drv mrk="ekzerc.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="ekzerc.0igxi.PED">
    <dif>
      Sin <tld/>i:
      <ekz>
        ili <tld/>i&gcirc;is &ccirc;iutage
        <fnt><bib>Fab1</bib>, <lok>Cikonioj</lok></fnt>;
      </ekz>
      <ekz>
        la elfknabinoj <tld/>i&gcirc;as en novaj dancoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sinjoro <klr>[&#8230;]</klr> montris arbaron de lancoj de la soldatoj, kiuj
        <tld/>i&gcirc;is en la korto
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;i en Esperanta parolado
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Tria Kongreso Esperantista en 
          Cambridge en la 12a de a&ubreve;gusto 1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        por forigi la timtremon estas tre konsilinde <tld/>i&gcirc;i kiom eble plej multe
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_k;&c_t;&c_y;&c_k;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trd lng="ca">exercitar-se</trd>
  <trd lng="de">sich <ind>&uuml;ben</ind></trd>
  <trdgrp lng="en">
    <trd>exercise</trd>,
    <trd>practice</trd>
  </trdgrp>  
  <trdgrp lng="es">
    <trd>entrenarse</trd>,
    <trd>ejercitarse</trd>  
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>exercer</ind><klr tip="ind">
    (s'entra&icirc;ner)</klr></trd>,
    <trd>s'<ind>entrainer</ind><klr tip="ind"> (s'exercer)</klr></trd>
  </trdgrp>
  <trd lng="he">&lamed;&he;&tav;&alef;&mem;&fnun;</trd>
  <trdgrp lng="pl">
    <trd>&cacute;wiczy&cacute; si&eogonek;</trd>,
    <trd>szkoli&cacute; si&eogonek;</trd>,
    <trd>trenowa&cacute;</trd>,
    <trd>wprawia&cacute; si&eogonek;</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_p;&c_r;&c_a;&c_zh;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="ekzerc.0iteco">
  <kap><tld/>iteco</kap>
  <snc mrk="ekzerc.0iteco.PED">
    <dif>
      <ref tip="super" cel="lert.0eco">Lerteco</ref> akirita de l' <tld/>i&gcirc;ado:
      <ekz>
        la ka&ubreve;zo de la balbutado ku&scirc;as ne en la lingvo, sed nur en la nesufi&ccirc;a
        <tld/>iteco de la diritaj personoj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista en Boulogne 
          sur Mer en la 5a de a&ubreve;gusto 1905</vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj fingroj perdis la <tld/>itecon kaj konfuzi&gcirc;adis sur la klavoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        nenio en la mondo <klr>[&#8230;]</klr> povis konkuri en la fleksi&gcirc;emeco de iliaj
        movoj, kun la elasteco de iliaj saltoj, kun la <klr>[&#8230;]</klr>
        perfekta <tld/>iteco de iliaj langoj
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_r;&c_a;&c_k;&c_t;&c_y;&c_k;&c_a;&c_v;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>habilitat</trd>,
    <trd>destresa</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>cvik</trd>,
    <trd>zb&ecaron;hlost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ge&uuml;btheit</trd>,
    <trd>Gewandtheit</trd>,
    <trd>Geschicklichkeit</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>facility (through practice)</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>adresse</trd>,
    <trd>dext&eacute;rit&eacute;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wyszkolenie</trd>,
    <trd>wprawa</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_n;&c_o;&c_r;&c_o;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_u;&c_m;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_n;&c_a;&c_t;&c_r;&c_je;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vycvi&ccaron;enos&tcaron;</trd>,
    <trd>zbehlos&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="ekzerc.stil0o">
  <kap>stil<tld/>o</kap>
  <snc mrk="ekzerc.stil0o.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      <tld lit="E"/>o de verkado celanta mastrigi ian apartan stilon, 
      ofte super nur formala temo:
      <ekz>
        <ctl>Stil<tld/>oj</ctl>
        <klr>(franclingva verko de <nom>R. Queneau</nom>)</klr>
        <fnt>
          <aut>R. Queneau, trad. I. Ertl</aut>,
          <vrk>Stilekzercoj</vrk>,
          <lok>La Kromkancerkliniko 4, 1986</lok>
        </fnt>;
      </ekz>
      <ekz>
        se anka&ubreve; miaj venontaj stil<tld/>oj estos bonaj, mi certe
        ricevos anka&ubreve; denove la noton &leftquot;bone&rightquot;
        <fnt><aut>U. Matthias</aut>,
        <vrk><url ref="http://www.u-matthias.de/verko/fajron.htm">
        Fajron sentas mi interne</url></vrk>,
        <lok>Pro Esperanto, 1990</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[&ccirc;u]</klr> temas pri publikigitaj partoj de iu granda romano a&ubreve; simple pri
        stil<tld/>oj
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 6 (56)</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">exercici d'estil</trd>
  <trd lng="de">Stil&uuml;bung</trd>
  <trdgrp lng="en">
    <trd>style exercise</trd>,
    <trd>exercise in style</trd>
  </trdgrp>
  <trd lng="fr">exercice de style</trd>
  <trd lng="es">ejercicio de estilo</trd>
</drv>
