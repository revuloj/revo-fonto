<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ekzerc</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ekzerc.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ekzerc.0i.PED">
    <dif>
      Lertigi per la praktiko:
      <ekz>
        <tld/>i la soldatojn
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 22</lok></fnt>;
      </ekz>
      <ekz>
        <ind>sin <tld/>i</ind> en parolado<fnt>Z</fnt>;
        <trd
        lng="be">&c_p;&c_r;&c_a;&c_k;&c_t;&c_y;&c_k;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
        <trd lng="ca">exercitar-se</trd>
        <trdgrp lng="es">
          <trd>entrenarse</trd>,
          <trd>ejercitarse</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>s'<ind>exercer</ind><klr tip="ind">
          (s'entra&icirc;ner)</klr></trd>,
          <trd>s'<ind>entra&icirc;ner</ind><klr tip="ind">
          (s'exercer)</klr></trd>
        </trdgrp>
        <trdgrp lng="pl">
          <trd>&cacute;wiczy&cacute; si&eogonek;</trd>,
          <trd>wprawia&cacute; si&eogonek;</trd>
        </trdgrp>
        <trd
        lng="ru">&c_u;&c_p;&c_r;&c_a;&c_zh;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
      </ekz>
      <ekz>
        <tld/>i sian orelon<fnt>Z</fnt>,
        sian langon, muskolojn, fingrojn;
      </ekz>
      <ekz>
        <tld/>i sian spiriton per la rezonado, sian memoron;
      </ekz>
      <ekz>
        vere <tld/>ita aktoro;
      </ekz>
      <ekz>
        niaj kongresoj estas <tld/>anta kaj edukanta
        anta&ubreve;parolo <klr>(enkonduko)</klr> por la estonta
        historio de la homaro<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dres.0i">dresi</ref>,
      <ref cel="hard.0i">hardi</ref>,
      <ref cel="prakti.0i">praktiki</ref>
    </refgrp>
    <rim>
      La esprimoj
      <ekz>
        <tld/>i lingvon, <tld/>i ian lernota&jcirc;on
      </ekz>
      a&ubreve; similaj estas malkorektaj &ccirc;ar la verbo
      <tld/>i, normale havas kiel rektan
      komplementon nur ian lernantan personon a&ubreve;
      plilerti&gcirc;ontan
      organon a&ubreve; beston.
    </rim>
  </snc>
  <trd
  lng="be">&c_p;&c_r;&c_a;&c_k;&c_t;&c_y;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">exercitar</trd>
  <trdgrp lng="de">
    <trd>&uuml;ben</trd>,
    <trd>exerzieren <klr>(soldatojn)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>exercise</trd>,
    <trd>train</trd>,
    <trd>drill</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>ejercitar</trd>,
    <trd>entrenar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>exercer<klr tip="ind"> (entra&icirc;ner)</klr></trd>,
    <trd>entra&icirc;ner<klr tip="ind"> (exercer)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&cacute;wiczy&cacute;</trd>,
    <trd>szkoli&cacute;</trd>,
    <trd>trenowa&cacute;</trd>,
    <trd>wprawia&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>exercitar</trd>,
    <trd>exercer</trd>,
    <trd>treinar</trd>,
    <trd>ensaiar</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_p;&c_r;&c_a;&c_zh;&c_n;&c_ja;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>cvičiť</trd>,
    <trd>nacvičovať</trd>
  </trdgrp>
</drv>

<drv mrk="ekzerc.0o">
  <kap><tld/>o</kap>
  <snc mrk="ekzerc.0o.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      Io por <tld/>i sin a&ubreve; aliajn:
      <ekz>
        gramatikaj, korpaj <tld/>oj;
      </ekz>
      <ekz>
        <tld/>o de legado
        <fnt><bib>F</bib>, <lok>&FE; 2</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd
  lng="be">&c_p;&c_r;&c_a;&c_k;&c_t;&c_y;&c_k;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="ca">exercici</trd>
  <trd lng="cs">cvi&ccaron;en&iacute;</trd>
  <trd lng="de">&Uuml;bung</trd>
  <trdgrp lng="en">
    <trd>exercise</trd>,
    <trd>drill</trd>
  </trdgrp>
  <trd lng="es">ejercicio</trd>
  <trd lng="fr">exercice</trd>
  <trdgrp lng="pl">
    <trd>&cacute;wiczenie</trd>,
    <trd>szkolenie</trd>,
    <trd>trening</trd>,
    <trd>wprawka</trd>
  </trdgrp>
  <trd
  lng="ru">&c_u;&c_p;&c_r;&c_a;&c_zh;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>cvik</trd>,
    <trd>cvičenie</trd>
  </trdgrp>
</drv>

<drv mrk="ekzerc.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="ekzerc.0ado.PED">
    <dif>
      Ago <tld/>i:
      <ekz>
        <tld/>ado povas &scirc;an&gcirc;i e&ccirc; la naturon
        mem<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_k;&c_t;&c_y;&c_k;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="ca">exercitaci&oacute;</trd>
  <trdgrp lng="en">
    <trd>exercise</trd>,
    <trd>practice</trd>,
    <trd>training</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>ejercicio</trd>,
    <trd>pr&aacute;ctica</trd>,
    <trd>entrenamiento</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>entra&icirc;nement</trd>,
    <trd>exercice<klr tip="ind"> (entra&icirc;nement)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&cacute;wiczenie</trd>,
    <trd>szkolenie</trd>,
    <trd>trening</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_p;&c_r;&c_a;&c_zh;&c_n;&c_je;&c_n;&c_i;&c_je;<klr
    tip="amb"> (&c_p;&c_r;&c_o;&c_c;&c_je;&c_s;&c_s;)</klr></trd>,
    <trd>&c_t;&c_r;&c_je;&c_n;&c_i;&c_r;&c_o;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>cvičenie</trd>,
    <trd>výcvik</trd>
  </trdgrp>
</drv>

<drv mrk="ekzerc.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="ekzerc.0aro.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      Libro enhavanta aron da <tld/>oj:
      <ekz>
        <tld/>aro de la lingvo internacia &leftquot;Esperanto&rightquot;
        <fnt><bib>F</bib>, <lok>&FE; 1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_b;&c_o;&c_r;&c_n;&c_ib;&c_k;
  <ind>&c_p;&c_r;&c_a;&c_k;&c_t;&c_y;&c_k;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ja;&c_w;</ind></trd>
  <trd lng="ca">llibre d'<ind>exercici</ind>s</trd>
  <trdgrp lng="en">
    <trd>exercise book</trd>,
    <trd>practice book</trd>,
    <trd>problem book</trd>
  </trdgrp>
  <trd lng="es"><ind>libro</ind> de ejercicios</trd>
  <trd lng="fr"><ind>recueil</ind> d'exercices</trd>
  <trdgrp lng="pl">
    <trd>&cacute;wiczenia<klr> (pot.)</klr></trd>,
    <trd>zeszyt <ind>&cacute;wicze&nacute;</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_b;&c_o;&c_r;&c_n;&c_i;&c_k;
  <ind>&c_u;&c_p;&c_r;&c_a;&c_zh;&c_n;&c_je;&c_n;&c_i;&c_j;</ind></trd>
  <trd lng="sk">cvičebnica</trd>
</drv>

<drv mrk="ekzerc.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="ekzerc.0igxi.PED">
    <dif>
      Sin <tld/>i:
      <ekz>
        ili <tld/>i&gcirc;is &ccirc;iutage
        <fnt><bib>Fab1</bib>, <lok>Cikonioj</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;i en esperanta parolado<fnt>Z</fnt>;
      </ekz>
      <ekz>
        oni multe devas <tld/>i&gcirc;adi anta&ubreve; ol veni al
        perfekteco.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_k;&c_t;&c_y;&c_k;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trd lng="ca">exercitar-se</trd>
  <trdgrp lng="en">
    <trd>exercise</trd>,
    <trd>practice</trd>
  </trdgrp>  
  <trdgrp lng="es">
    <trd>entrenarse</trd>,
    <trd>ejercitarse</trd>  
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>exercer</ind><klr tip="ind">
    (s'entra&icirc;ner)</klr></trd>,
    <trd>s'<ind>entrainer</ind><klr tip="ind"> (s'exercer)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&cacute;wiczy&cacute; si&eogonek;</trd>,
    <trd>szkoli&cacute; si&eogonek;</trd>,
    <trd>trenowa&cacute;</trd>,
    <trd>wprawia&cacute; si&eogonek;</trd>
  </trdgrp>
  <trd
  lng="ru">&c_u;&c_p;&c_r;&c_a;&c_zh;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="ekzerc.0iteco">
  <kap><tld/>iteco</kap>
  <snc mrk="ekzerc.0iteco.PED">
    <dif>
      Lerteco akirita de l' <tld/>i&gcirc;ado.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_r;&c_a;&c_k;&c_t;&c_y;&c_k;&c_a;&c_v;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>habilitat</trd>,
    <trd>destresa</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>facility (through practice)</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>adresse</trd>,
    <trd>dext&eacute;rit&eacute;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wyszkolenie</trd>,
    <trd>wprawa</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_n;&c_o;&c_r;&c_o;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_u;&c_m;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_n;&c_a;&c_t;&c_r;&c_je;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vycvičenosť</trd>,
    <trd>zbehlosť</trd>
  </trdgrp>
</drv>

<drv mrk="ekzerc.stil0o">
  <kap>stil<tld/>o</kap>
  <snc mrk="ekzerc.stil0o.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      <tld lit="E"/>o de redaktado celanta mastrigi ian apartan stilon, 
      ofte super nur formala temo:
      <ekz>
        <ctl>Stil<tld/>oj</ctl>
        <klr>(franclingva verko de R. Queneau)</klr> ;
        <fnt>
          <aut>R. Queneau, trad. I. Ertl</aut>,
          <vrk>Stilekzercoj</vrk>,
          <lok>La Kromkancerkliniko 4, 1986</lok>
        </fnt>.
      </ekz>
      <ekz>
        se anka&ubreve; miaj venontaj stil<tld/>oj estos bonaj, mi certe
        ricevos anka&ubreve; denove la noton &leftquot;bone&rightquot;
        <fnt><aut>U. Matthias</aut>,
        <vrk><url ref="http://www.u-matthias.de/verko/fajron.htm">
        Fajron sentas mi interne</url></vrk>,
        <lok>Pro Esperanto, 1990</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">exercici d'estil</trd>
  <trdgrp lng="en">
    <trd>style exercise</trd>,
    <trd>exercise in style</trd>
  </trdgrp>
  <trd lng="fr">exercice de style</trd>
  <trd lng="es">ejercicio de estilo</trd>
</drv>
</art>
