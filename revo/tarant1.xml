<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tarant1.xml,v 1.20 2019/07/25 14:10:14 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>tarantul</rad>/o
</kap>
<drv mrk="tarant1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="tarant1.0o.luparaneo">
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_specioj"
      cel="speci.0o.BIO">Specio</ref>
      de <ref tip="super" cel="arane.lup0o">luparaneo</ref>
      (<trd lng="la">Lycosa tarentula</trd>), helbruna kun nigraj strioj
      sur la dorso, vivanta en tertruoj en Mediteraneo.
    </dif>
    <bld
      lok="&WCU;/thumb/f/f3/Lycosa_tarentula_09924.jpg/320px-Lycosa_tarentula_09924.jpg"
      prm="CC BY-SA 3.0"
      ><tld/>o (Lycosa tarentula)
        <fnt>
          <aut>R. Benezet, propra verko</aut>
          <lok><url ref="&WCW;/File:Lycosa_tarentula_09924.jpg"
              >https://commons.wikimedia.org/wiki/File:Lycosa_tarentula_09924.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="be">&c_t;&c_a;&c_r;&c_a;&c_n;&c_t;&c_u;&c_l;</trd>
    <trd lng="ca">tar&agrave;ntula</trd>
    <trd lng="cs">tarantule</trd>
    <trd lng="de">Tarantel</trd>
    <trdgrp lng="fa">
     <trd>&a_r;&a_t;&f_y;&a_l;&a_A;&a_hamza;</trd>
     <trd>&a_r;&a_t;&f_y;&a_l;</trd>
    </trdgrp>
    <trd lng="fr">tarentule</trd>
    <trd lng="hu">tarantella(p&oacute;k)</trd>
    <trd lng="nl">tarantula</trd>
    <trd lng="pl">tarantula</trd>
    <trdgrp lng="pt">
     <trd>tar&acirc;ntula</trd>
    </trdgrp>
    <trd lng="ru">&c_t;&c_a;&c_r;&c_a;&c_n;&c_t;&c_u;&c_l;</trd>
    <trd lng="sk">tarantula</trd>
  </snc>
  <snc mrk="tarant1.0o.birdaraneo">
    <dif>
      Iuj grandaj araneoj el familio (<trd
      lng="la">Theraphosidae</trd>);
      birdaraneo:
      <ekz>
        la bird-man&gcirc;anta Goliato (a&ubreve; giganta <tld/>o) estas
        <klr>[&#8230;]</klr> ver&scirc;ajne la
        plej granda speco de araneo en la mondo, <klr>[&gcirc;i]</klr> vivas
        en la tropikaj
        &gcirc;angaloj de Suda Ameriko, kaj <klr>[&gcirc;iaj]</klr> kruroj povas
        etendi&gcirc;i &gcirc;is 30
        centimetroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=215085"
              >Bird-man&gcirc;anta Goliato</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Vogelspinne</trd>
  </snc>
</drv>
</art>
