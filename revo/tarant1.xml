<drv mrk="tarant1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="arane.lup0o">luparaneo</ref>
      (<trd lng="la">Lycosa tarentula</trd>), helbruna kun nigraj strioj
