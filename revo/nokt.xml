    <trd>de <ind>nit</ind>s</trd>
  </trdgrp>
  <trd lng="cs">v noci <klr>(adv.)</klr></trd>
  <trd lng="de">nachts</trd>
  <trdgrp lng="el">
    <trd>&eta; &ny;&ypsilon_ton;&kappa;&tau;&alfa;</trd>,
    <trd>&eta; &ny;&ypsilon_ton;&chi;&tau;&alfa;</trd>
  </trdgrp>
  <trd lng="en">night-time</trd>
  <trdgrp lng="fr">
    <trd>de <ind>nuit</ind></trd>,
    <trd>nuitamment</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;jjel</trd>,
    <trd>&eacute;jszaka</trd>
  </trdgrp>
  <trd lng="nl">'s <ind>nachts</ind></trd>
  <trd lng="pl">noc&aogonek;</trd>
  <trdgrp lng="pt">
    <trd>de <ind>noite</ind></trd>,
    <trd>&agrave; <ind>noite</ind></trd>
  </trdgrp>
  <trd lng="ro">noaptea</trd>
  <trd lng="ru">&c_n;&c_o;&c_ch;&c_mol;&c_ju;</trd>
  <trd lng="sk">v noci</trd>
  <trd lng="sv">p&aring; <ind>natten</ind></trd>
  <trd lng="tr">geceleyin</trd>
  <trd lng="uk">&c_u;&c_n;&c_o;&c_ch;&c_ib;</trd>
</drv>

<drv mrk="nokt.0i">
  <kap><tld/>i</kap>
  <snc mrk="nokt.0i.mallumi">
    <gra>sensubjekta</gra>
    <dif>
      Esti tempo de <tld/>o:
      <ekz>
        mi ekveturas vespere dum jam <tld/>as
        <fnt>
          <vrk><url ref="http://horizonte.free.fr/rakontoj/cirilo/07-nokte.html"
              >Nokte</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        atendas min esperantista gastiganto dum jam <tld/>as
        <fnt>
          <vrk><url ref="http://horizonte.free.fr/eventoj/bs/bs-2005-cirilo.html"
              >Bordozo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="br">beza&ntilde; <ind>noz</ind></trd>
    <trd lng="ca">ser de <ind>nit</ind></trd>
    <trd lng="en">it's <ind>night</ind> out</trd>
    <trd lng="de"><ind>Nacht</ind> sein</trd>
    <trd lng="es">ser de <ind>noche</ind></trd>
    <trd lng="fr">faire <ind>nuit</ind></trd>
    <trdgrp lng="hu">
      <trd>&eacute;jjel van</trd>,
      <trd>&eacute;jszaka van</trd>
    </trdgrp>
    <trd lng="nl">nacht zijn</trd>
    <trd lng="pl">sta&cacute; si&eogonek;  <ind>noc&aogonek;</ind></trd>
    <trdgrp lng="pt">
       <trd>ser <ind>noite</ind></trd>,
       <trd>ser de <ind>noite</ind></trd>
    </trdgrp>
    <trd lng="ro">s&abreve; devin&abreve;  <ind>noapte</ind></trd>
  </snc>
  <snc mrk="nokt.0i.tranokti">
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="sin" cel="nokt.tra0i">Tra<tld/>i</ref>:
      <ekz>
        en la vojo fari&gcirc;is nokto, kaj ili restis sur
        la kampo <tld/>i
        <fnt><bib>FK</bib>, <lok>3, anekdotoj</lok></fnt>;
      </ekz>
      <ekz>
        la komercistoj kaj vendistoj de &ccirc;iaspeca vendebla&jcirc;o <tld/>is
        ekstere de <nom>Jerusalem</nom>
        <fnt>
          <bib>MT</bib>
          <lok>Ne&hcirc;emja 13:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne <tld/>is fremdulo sur la strato
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 31:32</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u bubalo volos servi al vi, &ccirc;u &gcirc;i volos <tld/>i &ccirc;e via
        man&gcirc;ujo
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 39:9</lok>
        </fnt>?
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>n&auml;chtigen</trd>,
      <trd>&uuml;bernachten</trd>
    </trdgrp>
    <trd lng="nl">overnachten<klr tip="ind"> (ontr.)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>nocovat</trd>,
    <trd>p&rcaron;enocovat</trd>
  </trdgrp>
  <trd lng="pt">passar a <ind>noite</ind></trd>
  <trd lng="sk">prenocova&tcaron;</trd>
  <trd lng="uk">&c_n;&c_o;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="nokt.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <gra>sensubjekta</gra>
    <dif>
      Ekesti <tld/>o:
      <ekz>
        <tld/>i&gcirc;is, &scirc;i ekvidis malgrandan dometon kaj eniris por ripozi
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        jam <tld/>i&gcirc;is, la bela flor&gcirc;ardeno &scirc;ajnis nun griza
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la <tld/>i&gcirc;o.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">nozi&ntilde;</trd>
  <trdgrp lng="ca">
    <trd>fer-se de <ind>nit</ind></trd>,
    <trd>negrejar</trd>
  </trdgrp>
  <trd lng="de"><ind>Nacht</ind> werden</trd>
  <trd lng="el">&ny;&ypsilon;&chi;&tau;&omega_ton;&ny;&epsilon;&jota;</trd>
  <trd lng="en">night is falling</trd>
  <trd lng="es">anochecer</trd>
  <trd lng="fr">la <ind>nuit</ind> tombe</trd>
  <trdgrp lng="hu">
    <trd>lesz&aacute;ll az <ind>&eacute;j</ind></trd>,
    <trd>&eacute;jszaka lesz</trd>
  </trdgrp>
  <trd lng="kek">aak&#789;ab&#789;irk</trd>  
  <trd lng="nl">nacht worden</trd>
  <trd lng="pl">&sacute;ciemnia&cacute; si&eogonek;</trd>
  <trd lng="pt">anoitecer</trd>
  <trd lng="ro">&icirc;ntunecat&abreve;</trd>
</drv>

<drv mrk="nokt.tra0i">
  <kap>tra<tld/>i,
    <var><kap><tld/>umi</kap></var>
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Pasigi la <tld/>on, lo&gcirc;i dum <tld/>o:
      <ekz>
        tra<tld/>i en hotelo, sub tendo;
      </ekz>
      <ekz>
        ili venis en la tendaron kaj tra<tld/>is en la tendaro
        <fnt>
          <bib>MT</bib>
          <lok>Josuo 6:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili man&gcirc;is kaj trinkis kaj tra<tld/>is tie
        <fnt><bib>NT</bib>, <lok>&Jug; 19:4</lok></fnt>;
      </ekz>
      <ekz>
        li perdis la vojon, kaj petis la permeson tra<tld/>i &ccirc;e li
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        du migraj komizoj <klr>[&#8230;]</klr> devas <tld/>umi en la sama hotelo <klr>[&#8230;]</klr>
        <fnt>
          <bib>KrB</bib>
          <lok>En fremdan vazon ne &scirc;ovu la nazon</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu delira konstrua&jcirc;o de Herodo <klr>[&#8230;]</klr> min frenezigas, mi ne povas en &gcirc;i
        <tld/>umi
        <fnt>
          <bib>MkM</bib>
          <lok>25</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia amiko kaj mi tra<tld/>is en hamakoj, ligitaj inter
        arbotrunkoj
        <fnt>
          <bib>Monato</bib>
          <aut>Werner Schad</aut>
          <vrk><url ref="&Steloj;monato2/006868.html">Venezuelo:
           Inter maro kaj arbarego</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        tra<tld/>i en polica arestejo
        <fnt>
          <bib>Monato</bib>
          <aut>S. Maul</aut>
          <vrk><url ref="&Steloj;monato2/007154.html">Finfine
           oni povas dibo&ccirc;i sen pento</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_ch;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="br">nozvezhia&ntilde;</trd>
  <trdgrp lng="ca">
    <trd>fer <ind>nit</ind></trd>,
    <trd>pernoctar</trd>
  </trdgrp>
  <trd lng="cs">p&rcaron;espat</trd>
  <trdgrp lng="de">
    <trd>&uuml;bernachten</trd>
    <trd>n&auml;chtigenen</trd>
  </trdgrp>
  <trd lng="el">&delta;&jota;&alfa;&ny;&ypsilon;&kappa;&tau;&epsilon;&rho;&epsilon;
        &ypsilon_ton;&omega;</trd>
  <trd lng="en">spend the <ind>night</ind></trd>
  <trdgrp lng="es">
    <trd>pernoctar</trd>,
    <trd>trasnochar</trd>
  </trdgrp>
  <trd lng="fa">&a_sh;&a_b; &a_r;&a_A; &a_s;&f_p;&a_r;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr"><ind>passer</ind> la <ind>nuit</ind></trd>
  <trdgrp lng="hu">
    <trd>megsz&aacute;ll &eacute;jszak&aacute;ra</trd>,
    <trd>&eacute;jszak&aacute;zik</trd>,
    <trd>megh&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>meng<ind>inap</ind></trd>,
    <trd>ber<ind>malam</ind></trd>
  </trdgrp>
  <trd lng="nl">overnachten</trd>
  <trd lng="pl">przenocowa&cacute;</trd>
  <trd lng="pt">pernoitar</trd>
  <trdgrp lng="ro">
    <trd>mut&abreve;</trd>,
    <trd>&icirc;nnopta</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_o;&c_ch;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_n;&c_o;&c_ch;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">prespa&tcaron;</trd>
  <trd lng="tr">geceyi ge&ccedil;irmek</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_n;&c_o;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_n;&c_o;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="nokt.tra0ejo">
  <kap>
    tra<tld/>ejo
  </kap>
  <snc>
    <dif>
      Loko, kie oni povas tra<tld/>i:
      <ekz>
        <klr>[ili &ccirc;i tie]</klr> gastigemaj kaj espereble mi sukcesos akiri tra<tld/>ejon
        <fnt>
          <bib>VaK</bib>
          <lok>Sesa &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la katolika eklezio prizorgas la manifestaciantojn, kaj kelkaj akceptis lo&gcirc;i en
        provizoraj tra<tld/>ejoj
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="https://www.monato.be/2013/010993.php"
              >Pre&gcirc;ejo rifu&gcirc;ejo</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        vizito de la lernejoj kaj ties tra<tld/>ejoj
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470; 11</lok>
        </fnt>;
      </ekz>
      <ekz>
        facile oni trovos senpagan tra<tld/>ejon &ccirc;e esperantistoj de Francio
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#8470; 4 (102)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="gast.0ejo">gastejo</ref>,
      <ref cel="hotel.0o">hotelo</ref>,
      <ref cel="kamp.0adejo">kampadejo</ref>,
      <ref cel="kvartir.0o">kvartiro</ref>,
      <ref cel="pensio.0o.gastejo">pensiono<sncref/></ref>,
      <ref cel="tend.0umejo">tendumejo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Unterkunft</trd>,
    <trd>&Uuml;bernachtungsm&ouml;glichkeit</trd>
  </trdgrp>
  <trd lng="en">accomodation</trd>
  <trd lng="fr">h&eacute;bergement</trd>
</drv>









<drv mrk="nokt.tag0o">
  <kap>tag<tld/>o,
  <var><kap>tago<tld/>o</kap></var></kap>
  <snc mrk="nokt.tag0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      <ref tip="dif" cel="diurn.0o.KAL">Diurno</ref>:
      <ekz>
        kun ili mi eniros Labirinton en tag<tld/>o anta&ubreve; la atako
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kvarpiedulo estis &scirc;losita sen nutra&jcirc;o kaj akvo dum 17
        tag<tld/>oj kaj tamen restis viva
        <fnt>
          <bib>Monato</bib>
          <aut>AMik</aut>
          <vrk><url ref="&Monato;/2007/009134.php">Delikate
          ...</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        tri tag<tld/>ojn oni bezonis, &gcirc;is finfine la aviadilon oni
        trovis
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2015/011535.php">Malsukcesa
          aviado</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la batalo da&ubreve;ris ankora&ubreve; du tago<tld/>ojn
        <fnt>
          <aut>J. Francis</aut>
          <vrk>La Granda Kaldrono</vrk>
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_t;&c_k;&c_ib;</trd>
  <trd lng="br">deiz<klr tip="ind"> (24 eur)</klr></trd>
  <trdgrp lng="ca">
    <trd>dia solar</trd>,
    <trd>dia sencer</trd>
  </trdgrp>
  <trd lng="cs">den = 24 hod.</trd>
  <trd lng="de">Tag<klr tip="ind"> (24 Stunden)</klr></trd>
  <trd lng="en"><klr>(24-hour) </klr>day</trd>
  <trd lng="fa">&a_sh;&a_b;&a_A;&a_n;&a_h;&zwnj;&a_r;&a_w;&a_z;</trd>
  <trdgrp lng="fr">
    <trd>jour<klr tip="ind"> (24 heures)</klr></trd>,
    <trd>nycth&eacute;m&egrave;re</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nap&eacute;j</trd>,
    <trd>&eacute;jjel &eacute;s nappal</trd>
  </trdgrp>
  <trd lng="nl">etmaal</trd>
  <trd lng="pl">doba</trd>
  <trdgrp lng="pt">
    <trd>dia e <ind>noite</ind> <klr tip="amb">(24 horas)</klr></trd>,
    <trd>noite e <ind>dia</ind> <klr tip="amb">(24 horas)</klr></trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>douazeci si patru de ore</trd>,
    <trd>zi <klr>(24 ore)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_u;&c_t;&c_k;&c_i;</trd>
  <trd lng="sk">de&ncaron; = 24 hod&iacute;n</trd>
  <trd lng="sv">dygn</trd>
  <trd lng="tr">g&uuml;n<klr>(24 saat)</klr></trd>
  <trd lng="uk">&c_d;&c_o;&c_b;&c_a;</trd>
</drv>

<drv mrk="nokt.0mezo">
  <kap>
    <tld/>mezo
  </kap>
  <snc>
    <dif>
      La mezo inter sunsubiro kaj sunlevi&gcirc;o, je la nula <klr>(dudek
      kvara)</klr> horo:
      <ekz>
        <nom>&Scirc;im&scirc;on</nom> ku&scirc;is &gcirc;is <tld/>omezo, je <tld/>omezo li levi&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 16:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        la horlo&gcirc;o sonis <tld/>omezon
        <fnt>
          <bib>Fab1</bib>
          <lok>Persista stana soldato</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne estas ankora&ubreve; <tld/>omezo, respondis la knabino, sed jam
        pasis la dek unua horo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>;
      </ekz>
      <ekz>
        je <tld/>omezo oni okazigis artfajra&jcirc;on, kia &gcirc;i <klr>[&#8230;]</klr>
        estis ankora&ubreve; neniam vidita
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Ar&gcirc;enta Urbo Amarganto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Mitternacht</trd>
  <trd lng="en">midnight</trd>
  <trd lng="fa">&a_n;&f_y;&a_m;&a_h;&zwnj;&a_sh;&a_b;</trd>
  <trd lng="fr">minuit</trd>
  <trd lng="pl">p&oacute;&lstroke;noc <klr tip="amb">(godzina)</klr></trd>
  <trd lng="ro">miezul <ind>nop&tcedil;ii</ind></trd>
</drv>



