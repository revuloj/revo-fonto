</drv>

<drv mrk="nokt.tag0o">
  <kap>tag<tld/>o,
  <var><kap>tago<tld/>o</kap></var></kap>
  <snc mrk="nokt.tag0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      <ref tip="dif" cel="diurn.0o.KAL">Diurno</ref>:
      <ekz>
        kun ili mi eniros Labirinton en tag<tld/>o anta&ubreve; la atako
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kvarpiedulo estis &scirc;losita sen nutra&jcirc;o kaj akvo dum 17
        tag<tld/>oj kaj tamen restis viva
        <fnt>
          <bib>Monato</bib>
          <aut>AMik</aut>
          <vrk><url ref="&Monato;/2007/009134.php">Delikate
          ...</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        tri tag<tld/>ojn oni bezonis, &gcirc;is finfine la aviadilon oni
        trovis
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2015/011535.php">Malsukcesa
          aviado</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la batalo da&ubreve;ris ankora&ubreve; du tago<tld/>ojn
        <fnt>
          <aut>J. Francis</aut>
          <vrk>La Granda Kaldrono</vrk>
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_t;&c_k;&c_ib;</trd>
  <trd lng="br">deiz<klr tip="ind"> (24 eur)</klr></trd>
  <trdgrp lng="ca">
    <trd>dia solar</trd>,
    <trd>dia sencer</trd>
  </trdgrp>
  <trd lng="cs">den = 24 hod.</trd>
  <trd lng="de">Tag<klr tip="ind"> (Zeitraum von 24 Stunden)</klr></trd>
  <trd lng="en"><klr>(24-hour) </klr>day</trd>
  <trd lng="fa">&a_sh;&a_b;&a_A;&a_n;&a_h;&zwnj;&a_r;&a_w;&a_z;</trd>
  <trdgrp lng="fr">
    <trd>jour<klr tip="ind"> (24 heures)</klr></trd>,
    <trd>nycth&eacute;m&egrave;re</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nap&eacute;j</trd>,
    <trd>&eacute;jjel &eacute;s nappal</trd>
  </trdgrp>
  <trd lng="nl">etmaal</trd>
  <trd lng="pl">doba</trd>
  <trdgrp lng="pt">
    <trd>dia e <ind>noite</ind> <klr tip="amb">(24 horas)</klr></trd>,
    <trd>noite e <ind>dia</ind> <klr tip="amb">(24 horas)</klr></trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>douazeci si patru de ore</trd>,
    <trd>zi <klr>(24 ore)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_u;&c_t;&c_k;&c_i;</trd>
  <trd lng="sk">de&ncaron; = 24 hod&iacute;n</trd>
  <trd lng="sv">dygn</trd>
  <trd lng="tr">g&uuml;n<klr>(24 saat)</klr></trd>
</drv>

<drv mrk="nokt.0mezo">
  <kap>
    <tld/>mezo
  </kap>
  <snc>
    <dif>
      La mezo inter sunsubiro kaj sunlevi&gcirc;o, je la nula <klr>(dudek
      kvara)</klr> horo:
      <ekz>
        &Scirc;im&scirc;on ku&scirc;is &gcirc;is <tld/>omezo, je <tld/>omezo li levi&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 16:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        la horlo&gcirc;o sonis <tld/>omezon
        <fnt>
          <bib>Fab1</bib>
          <lok>Persista stana soldato</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne estas ankora&ubreve; <tld/>omezo, respondis la knabino, sed jam
        pasis la dek unua horo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>;
      </ekz>
      <ekz>
        je <tld/>omezo oni okazigis artfajra&jcirc;on, kia &gcirc;i <klr>[&#x2026;]</klr>
        estis ankora&ubreve; neniam vidita
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Ar&gcirc;enta Urbo Amarganto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Mitternacht</trd>
  <trd lng="en">midnight</trd>
  <trd lng="fa">&a_n;&f_y;&a_m;&a_h;&zwnj;&a_sh;&a_b;</trd>
  <trd lng="fr">minuit</trd>
</drv>



