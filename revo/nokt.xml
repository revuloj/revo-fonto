<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>nokt</rad>/o
</kap>
<drv mrk="nokt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Tempo dum kiu la suno estas sub horizonto kaj ne lumigas al ni:
      <ekz>
        stelplena <tld/>o;
      </ekz>
      <ekz>
        la <tld/>o falas <klr>(komenci&gcirc;as)</klr>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Mallumo materia a&ubreve; spirita:
      <ekz>
        la <tld/>o de la tombo;
      </ekz>
      <ekz>
        blindeco estas senfina <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_o;&c_ch;</trd>
  <trd lng="cs">noc</trd>
  <trd lng="de">Nacht</trd>
  <trdgrp lng="el">
    <trd>&ny;&ypsilon_ton;&kappa;&tau;&alfa;</trd>,
    <trd>&ny;&ypsilon_ton;&chi;&tau;&alfa;</trd>
  </trdgrp>
  <trd lng="en">night</trd>
  <trd lng="fr">nuit</trd>
  <trd lng="es">noche</trd>
  <trd lng="nl">nacht</trd>
  <trd lng="pt">noite</trd>
  <trd lng="ru">&c_n;&c_o;&c_ch;&c_mol;</trd>
  <trd lng="sv">natt</trd>
  <trdgrp lng="zh">
    <trd>&#22812;<klr> [y&egrave;]</klr></trd>,
    <trd>&#23477;<klr> [xi&#257;o]</klr></trd>
  </trdgrp>
</drv>

<drv mrk="nokt.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Dum <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_n;&c_a;&c_ch;&c_y;</trd>,
    <trd>&c_n;&c_o;&c_ch;&c_ch;&c_u;</trd>
  </trdgrp>
  <trd lng="de">nachts</trd>
  <trdgrp lng="el">
    <trd>&eta; &ny;&ypsilon_ton;&kappa;&tau;&alfa;</trd>,
    <trd>&eta; &ny;&ypsilon_ton;&chi;&tau;&alfa;</trd>
  </trdgrp>
  <trd lng="en">night-time</trd>
  <trd lng="nl">'s <ind>nachts</ind></trd>
  <trd lng="pt">de noite</trd>
  <trd lng="ru">&c_n;&c_o;&c_ch;&c_mol;&c_ju;</trd>
  <trd lng="sv">p&aring; <ind>natten</ind></trd>
</drv>

<drv mrk="nokt.0as">
  <kap><tld/>as</kap>
  <snc>
    <dif>
      Estas <tld/>o.
    </dif>
  </snc>
  <trd lng="en">it's <ind>night</ind> out</trd>
</drv>

<drv mrk="nokt.0i">
  <kap><tld/>i<fnt>Z</fnt>, <tld/>adi<fnt>Z</fnt></kap>
  <uzo tip="stl">EVI</uzo>
  <snc>
    <dif>
      <ref tip="sin" cel="nokt.tra0i">Tra<tld/>i</ref>.
    </dif>
  </snc>
</drv>

<drv mrk="nokt.0igxas">
  <kap><tld/>i&gcirc;as</kap>
  <snc>
    <dif>
      <tld lit="N"/>o komenci&gcirc;as:
      <ekz>
        &ccirc;e la <tld/>i&gcirc;o.
      </ekz>
    </dif>
  </snc>
  <trd lng="el">&ny;&ypsilon;&chi;&tau;&omega_ton;&ny;&epsilon;&jota;</trd>
  <trd lng="en">night is falling</trd>
  <trd lng="pt">anoitece</trd>
</drv>

<drv mrk="nokt.tra0i">
  <kap>tra<tld/>i, <tld/>oresti<fnt>Z</fnt></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Pasigi la <tld/>on, lo&gcirc;i dum <tld/>o:
      <ekz>
        tra<tld/>i en hotelo, sub tendo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_ch;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">&uuml;bernachten</trd>
  <trd
lng="el">&delta;&jota;&alfa;&ny;&ypsilon;&kappa;&tau;&epsilon;&rho;&epsilon;&ypsilon_ton;&omega;</trd>
  <trd lng="en">spend the <ind>night</ind></trd>
  <trd lng="nl">overnachten</trd>
  <trd lng="pt">pernoitar</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_o;&c_ch;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_n;&c_o;&c_ch;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="nokt.tag0o">
  <kap>tag<tld/>o</kap>
  <snc>
    <dif>
      Da&ubreve;ro de 24 horoj.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_t;&c_k;&c_ib;</trd>
  <trd lng="en"><klr>(24-hour) </klr>day</trd>
  <trd lng="nl">etmaal</trd>
  <trd lng="ru">&c_s;&c_u;&c_t;&c_k;&c_i;</trd>
  <trd lng="sv">dygn</trd>
</drv>

<drv mrk="nokt.tag0egaleco">
  <kap>tag<tld/>egaleco</kap>
  <snc>
    <dif>
      <ref tip="sin" cel="ekvino.0o">Ekvinokso</ref>.
    </dif>
  </snc>
  <trd
lng="be">&c_r;&c_o;&c_w;&c_n;&c_a;&c_d;&c_z;&c_jo;&c_n;&c_s;&c_t;&c_v;&c_a;</trd>
  <trd lng="nl">nachtevening</trd>
  <trd lng="en">equinox</trd>
  <trd
lng="ru">&c_r;&c_a;&c_v;&c_n;&c_o;&c_d;&c_je;&c_n;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
</drv>
</art>
