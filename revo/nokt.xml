<drv mrk="nokt.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Dum <tld/>o:
      <ekz>
        iri tage kaj <tld/>e
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 13:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi timus hodia&ubreve; <tld/>e dormi sola
        <fnt>
          <bib>Fab3</bib>
          <lok>Estas tute certe</lok>
        </fnt>;
      </ekz>
      <ekz>
        levi&gcirc;is kontra&ubreve; min la lo&gcirc;antoj de Gibea, kaj &ccirc;irka&ubreve;is pro
        mi la
        domon <tld/>e
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 20:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi levi&gcirc;is <tld/>e
        <fnt>
          <bib>MT</bib>
          <lok>Ne&hcirc;emja 2:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        post la dekunua horo <tld/>e
        <fnt>
          <bib>Fab4</bib>
          <lok>Peco da perlovico</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni invitas vin por hodia&ubreve; <tld/>e
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        li vivis gaje, li iradis &ccirc;iu<tld/>e al maskobaloj
        <fnt>
          <bib>Fab1</bib>
          <lok>Fluganta kofro</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i &gcirc;uis trankvilon nek tage, nek <tld/>e
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; ili tage kaj <tld/>e brulas seninterrompe dekoj kaj
        centoj da multekostaj lampionoj
        <fnt><bib>&Ccirc;uk&ccirc;oj</bib>,
          <lok>blindaj &ccirc;uk&ccirc;oj, (du skizoj), unua
            skizo, Filip Onkudimov, 1</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        brilplenaj bildoj sur <tld/>e malluma fono
        <fnt>
          <bib>Fab3</bib>
          <lok>Dorna vojo de la honoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>enigra densa kurteno
        <fnt>
          <bib>Fab4</bib>
          <lok>Infano en la tombo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_n;&c_a;&c_ch;&c_y;</trd>,
    <trd>&c_n;&c_o;&c_ch;&c_ch;&c_u;</trd>
  </trdgrp>
  <trd lng="bo">&#xF51;&#xF7C;&#xF0B;&#xF51;&#xF42;&#xF7C;&#xF44;&#xF0B;</trd>
  <trd lng="br">diouzh an <ind>noz</ind></trd>
  <trdgrp lng="ca">
    <trd>de <ind>nit</ind></trd>,
    <trd>de <ind>nit</ind>s</trd>
  </trdgrp>
  <trd lng="cs">v noci <klr>(adv.)</klr></trd>
  <trd lng="de">nachts</trd>
  <trdgrp lng="el">
    <trd>&eta; &ny;&ypsilon_ton;&kappa;&tau;&alfa;</trd>,
    <trd>&eta; &ny;&ypsilon_ton;&chi;&tau;&alfa;</trd>
  </trdgrp>
  <trd lng="en">night-time</trd>
  <trdgrp lng="fr">
    <trd>de <ind>nuit</ind></trd>,
    <trd>nuitamment</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;jjel</trd>,
    <trd>&eacute;jszaka</trd>
  </trdgrp>
  <trd lng="nl">'s <ind>nachts</ind></trd>
  <trd lng="pl">noc&aogonek;</trd>
  <trdgrp lng="pt">
    <trd>de <ind>noite</ind></trd>,
    <trd>&agrave; <ind>noite</ind></trd>
  </trdgrp>
  <trd lng="ro">noaptea</trd>
  <trd lng="ru">&c_n;&c_o;&c_ch;&c_mol;&c_ju;</trd>
  <trd lng="sk">v noci</trd>
  <trd lng="sv">p&aring; <ind>natten</ind></trd>
  <trd lng="tr">geceleyin</trd>
</drv>

<drv mrk="nokt.0i">
  <kap><tld/>i</kap>
  <snc mrk="nokt.0i.mallumi">
    <gra>sensubjekta</gra>
    <dif>
      Esti tempo de <tld/>o.
    </dif>
    <trd lng="br">beza&ntilde; <ind>noz</ind></trd>
    <trd lng="ca">ser de <ind>nit</ind></trd>
    <trd lng="en">it's <ind>night</ind> out</trd>
    <trd lng="de">Nacht sein</trd>
    <trd lng="es">ser de <ind>noche</ind></trd>
    <trd lng="fr">faire <ind>nuit</ind></trd>
    <trdgrp lng="hu">
      <trd>&eacute;jjel van</trd>,
      <trd>&eacute;jszaka van</trd>
    </trdgrp>
    <trd lng="nl">nacht zijn</trd>
    <trdgrp lng="pt">
       <trd>ser <ind>noite</ind></trd>,
       <trd>ser de <ind>noite</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="nokt.0i.tranokti">
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="sin" cel="nokt.tra0i">Tra<tld/>i</ref>:
      <ekz>
        en la vojo fari&gcirc;is nokto, kaj ili restis sur
        la kampo <tld/>i
        <fnt><bib>FK</bib>, <lok>3, anekdotoj</lok></fnt>;
      </ekz>
      <ekz>
        la komercistoj kaj vendistoj de &ccirc;iaspeca vendebla&jcirc;o <tld/>is
        ekstere de Jerusalem
        <fnt>
          <bib>MT</bib>
          <lok>Ne&hcirc;emja 13:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne <tld/>is fremdulo sur la strato
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 31:32</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u bubalo volos servi al vi, &ccirc;u &gcirc;i volos <tld/>i &ccirc;e via
        man&gcirc;ujo?

        <fnt>
          <bib>MT</bib>
          <lok>Ijob 39:9</lok>
        </fnt>
      </ekz>
    </dif>
    <trd lng="de">n&auml;chtigen</trd>
    <trd lng="nl">overnachten<klr tip="ind"> (ontr.)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>nocovat</trd>,
    <trd>p&rcaron;enocovat</trd>
  </trdgrp>
  <trd lng="de">&uuml;bernachten</trd>
  <trd lng="pt">passar a <ind>noite</ind></trd>
  <trd lng="sk">prenocova&tcaron;</trd>
</drv>

<drv mrk="nokt.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <gra>sensubjekta</gra>
    <dif>
      Ekesti <tld/>o:
      <ekz>
        <tld/>i&gcirc;is, &scirc;i ekvidis malgrandan dometon kaj eniris por ripozi
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        jam <tld/>i&gcirc;is, la bela flor&gcirc;ardeno &scirc;ajnis nun griza
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la <tld/>i&gcirc;o.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">nozi&ntilde;</trd>
  <trdgrp lng="ca">
    <trd>fer-se de <ind>nit</ind></trd>,
    <trd>negrejar</trd>
  </trdgrp>
  <trd lng="de">Nacht werden</trd>
  <trd lng="el">&ny;&ypsilon;&chi;&tau;&omega_ton;&ny;&epsilon;&jota;</trd>
  <trd lng="en">night is falling</trd>
  <trd lng="es">anochecer</trd>
  <trd lng="fr">la <ind>nuit</ind> tombe</trd>
  <trdgrp lng="hu">
    <trd>lesz&aacute;ll az <ind>&eacute;j</ind></trd>,
    <trd>&eacute;jszaka lesz</trd>
  </trdgrp>
  <trd lng="nl">nacht worden</trd>
  <trd lng="pl">&sacute;ciemnia&cacute; si&eogonek;</trd>
  <trd lng="pt">anoitecer</trd>
  <trd lng="kek">aak&#x315;ab&#x315;irk</trd>
</drv>

<drv mrk="nokt.tra0i">
  <kap>tra<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Pasigi la <tld/>on, lo&gcirc;i dum <tld/>o:
      <ekz>
        tra<tld/>i en hotelo, sub tendo;
      </ekz>
      <ekz>
        ili venis en la tendaron kaj tra<tld/>is en la tendaro
        <fnt>
          <bib>MT</bib>
          <lok>Josuo 6:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili man&gcirc;is kaj trinkis kaj tra<tld/>is tie
        <fnt><bib>NT</bib>, <lok>&Jug; 19:4</lok></fnt>;
      </ekz>
      <ekz>
        li perdis la vojon, kaj petis la permeson tra<tld/>i &ccirc;e li
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia amiko kaj mi tra<tld/>is en hamakoj, ligitaj inter
        arbotrunkoj
        <fnt>
          <bib>Monato</bib>
          <aut>Werner Schad</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006868.html">Venezuelo:
           Inter maro kaj arbarego</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        tra<tld/>i en polica arestejo
        <fnt>
          <bib>Monato</bib>
          <aut>S. Maul</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007154.html">Finfine
           oni povas dibo&ccirc;i sen pento</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_ch;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="br">nozvezhia&ntilde;</trd>
  <trdgrp lng="ca">
    <trd>fer <ind>nit</ind></trd>,
    <trd>pernoctar</trd>
  </trdgrp>
  <trd lng="cs">p&rcaron;espat</trd>
  <trdgrp lng="de">
    <trd>&uuml;bernachten</trd>
    <trd>n&auml;chtigenen</trd>
  </trdgrp>
  <trd lng="el">&delta;&jota;&alfa;&ny;&ypsilon;&kappa;&tau;&epsilon;&rho;&epsilon;
        &ypsilon_ton;&omega;</trd>
  <trd lng="en">spend the <ind>night</ind></trd>
  <trdgrp lng="es">
    <trd>pernoctar</trd>,
    <trd>trasnochar</trd>
  </trdgrp>
  <trd lng="fa">&a_sh;&a_b; &a_r;&a_A; &a_s;&f_p;&a_r;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr"><ind>passer</ind> la <ind>nuit</ind></trd>
  <trdgrp lng="hu">
    <trd>megsz&aacute;ll &eacute;jszak&aacute;ra</trd>,
    <trd>&eacute;jszak&aacute;zik</trd>,
    <trd>megh&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>meng<ind>inap</ind></trd>,
    <trd>ber<ind>malam</ind></trd>
  </trdgrp>
  <trd lng="nl">overnachten</trd>
  <trd lng="pl">przenocowa&cacute;</trd>
  <trd lng="pt">pernoitar</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_o;&c_ch;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_n;&c_o;&c_ch;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
