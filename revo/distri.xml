<drv mrk="distri.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="distri.0i.KOMUNE">
    <dif>
      Dis<ref cel="divid.0i.disdoni">dividi</ref>, <ref tip="vid" cel="don.dis0i">disdoni</ref>,
      dispartigi:
      <ekz>
        <klr>[ili]</klr> alportadis dekona&jcirc;on el la greno, <klr>[kaj]</klr> ili havis la
        devon <tld/>i la partojn al siaj fratoj
        <fnt>
          <bib>MT</bib>
          <lok>Ne&hcirc;emja 13:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> <tld/>is &ccirc;ion egalmezure
        <fnt>
          <bib>Fab3</bib>
          <lok>Ekzistas diferenco</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> <tld/>is frapojn
        <fnt>
          <bib>SrT</bib>
          <lok>Libro IX.</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ie oni <tld/>is la tekston de la Deklaracio
        <fnt>
          <bib>EeP</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        Supera Helena Esperanta Instituto <klr>[&#x2026;]</klr> <tld/>as diplomojn en tri gradoj
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni jam ekis <tld/>i la man&gcirc;a&jcirc;ojn! kaj mi idioto jen senutile medita&ccirc;as
        <fnt>
          <bib>SkandalJozef</bib>
          <lok>Korpogvardia veterano</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>us terparcelojn inter la popolo
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Ok</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is monon, pretekstante, ke tiuj sumoj estas kontribua&jcirc;oj al
        elektokampanjaj elspezoj
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007075.html">Pa&scirc;oj al purigado</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ili a&ccirc;etis la varojn eksterlande kaj <tld/>is ilin en E&ubreve;ropo
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007334.html">Belgio: Lernout kaj Hauspie: revo fini&gcirc;is</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la usonaj film<tld/>antaj kompanioj ne permesas <tld/>i la ne-holivudajn filmojn
        <fnt>
          <bib>Monato</bib>
          <aut>Miguel Boeira Vianna</aut>
          <vrk><url ref="https://www.monato.be/2003/008118.html">El fatraso ekstremdekstremula</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bankoj emisiis, <tld/>is kaj &scirc;an&gcirc;is monon, sed ne rajtis postuli interezojn
        la&ubreve; la admonoj de Korano
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="https://www.monato.be/2004/008265.html">Parakosmo</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        la elektra reto komence estis koncipita por transporti kaj <tld/>i la elektron
        de kelkaj grandaj centraloj en Belgio al &ccirc;iuj uzantoj
        <fnt>
          <bib>Monato</bib>
          <aut>Eddy Van den Bosch</aut>
          <vrk><url ref="https://www.monato.be/2014/011380.php">&Ccirc;u sabotado &ccirc;e nuklea centralo?</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="deploj.0i">deploji</ref>,
      <ref cel="vend.dis0i">disvendi</ref>,
      <ref cel="don.el0i">eldoni</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_m;&c_ja;&c_r;&c_k;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_w;&c_s;&c_ju;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_j;&c_m;&c_a;&c_c;&c_c;&c_a; <ind>&c_d;&c_y;&c_s;&c_t;&c_r;&c_y;&c_b;&c_u;&c_c;&c_y;&c_ja;&c_j;</ind></trd>
  </trdgrp>
  <trd lng="bo">&t_ga;&t_rata;&t_drengbu;&t_ma;&t_sa;&t_tsheg;&t_-a;&t_ga;&t_rata;&t_drengbu;&t_ma;&t_sa;&t_tsheg;&t_ba;&t_ka;&t_rata;&t_ma;&t_sa;&t_tsheg;&t_kha;&t_rata;&t_naro;&t_ma;&t_sa;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>distribuovat</trd>,
    <trd>rozd&ecaron;lit</trd>,
    <trd>rozm&iacute;stit</trd>,
    <trd>rozv&aacute;d&ecaron;t</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>austeilen</trd>,
    <trd>verteilen</trd>,
    <trd>distribuieren</trd>
  </trdgrp>
  <trd lng="en">distribute</trd>
  <trd lng="es">distribuir</trd>
  <trdgrp lng="fr">
    <trd>distribuer</trd>,
    <trd>r&eacute;partir</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5206;&#x914D;&#x3059;&#x308B;  <pr>&#x3076;&#x3093;&#x3071;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5272;&#x308A;&#x5F53;&#x3066;&#x308B;  <pr>&#x308F;&#x308A;&#x3042;&#x3066;&#x308B;</pr></trd>,
    <trd>&#x914D;&#x9054;&#x3059;&#x308B;  <pr>&#x306F;&#x3044;&#x305F;&#x3064;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x914D;&#x5E03;&#x3059;&#x308B;  <pr>&#x306F;&#x3044;&#x3075;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dystrybuowa&cacute;</trd>,
    <trd>rozprowadza&cacute;</trd>,
    <trd>rozdziela&cacute;</trd>,
    <trd>roznosi&cacute;</trd>,
    <trd>rozdawa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">distribuir</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_r;&c_je;&c_d;&c_je;&c_l;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>pride&lcaron;ova&tcaron;</trd>,
    <trd>rozde&lcaron;ova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_z;&c_p;&c_o;&c_d;&c_ib;&c_l;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_o;&c_sh;&c_i;&c_r;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_p;&c_o;&c_v;&c_s;&c_ju;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6563;&#x51FA; <pr>s&#xE0;nch&#x16B;</pr></trd>,
    <trd>&#x64AD;&#x53D1; <pr>b&#xF2;f&#x101;</pr></trd>,
    <trd>&#x7D93;&#x92B7; <pr>j&#x12B;ngxi&#x101;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="distri.0o">
  <kap><tld/>o</kap>
  <snc mrk="distri.0o.ago">
    <dif>
      Disdonado:
      <ekz>
        <tld/>o de la taskoj
        <fnt>
          <bib>EeP</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de tramaj kaj a&ubreve;tobusaj biletoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la dokumentoj, kiuj devas esti eldonitaj en &ccirc;iuj oficialaj lingvoj, ka&ubreve;zas
        <klr>[&#x2026;]</klr> e&ccirc; pure teknikan konfuzon en la <tld/>o
        <fnt>
          <bib>EeP</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de respondecoj inter la centra hispana kaj la a&ubreve;tonoma kataluna
        registaroj
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="https://www.monato.be/2006/008812.php">Survoje al pli granda memregado</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="distri.0o.SPA">
    <dif>
      Dislokado en spaco a&ubreve; populacio:
      <ekz>
        geografia <tld/>o
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de termamuloj anta&ubreve;e estis malsama
        al tio kio &gcirc;i nun estas
        <fnt>
          <bib>OriginoDeSpecioj</bib>,
          <lok>&ccirc;apitro 10a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de sango-grupoj de hungaroj kaj nordaj &ccirc;inoj samas
        <fnt>
          <bib>Monato</bib>
          <aut>Karlo Juh&aacute;sz</aut>
          <vrk><url ref="https://www.monato.be/2010/009995.php">Pordo al okcidento</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        alpreni efikajn rimedojn por ne lasi la diferencon de la salajra <tld/>o
        pligrandi&gcirc;i
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="https://www.monato.be/2008/009369.php">La nova &Ccirc;inio estas ekonomie sukcesa</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dispozici.0o.arangxo">dispozicio<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_w;&c_s;&c_ju;&c_d;&c_zh;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_m;&c_je;&c_r;&c_k;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_d;&c_a;&c_ch;&c_a;</trd>,
    <trd>&c_d;&c_y;&c_s;&c_t;&c_r;&c_y;&c_b;&c_u;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>distribuce</trd>,
    <trd>rozd&ecaron;lov&aacute;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>distribution</trd>,
    <trd>r&eacute;partition</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5206;&#x914D;  <pr>&#x3076;&#x3093;&#x3071;&#x3044;</pr></trd>,
    <trd>&#x5272;&#x5F53;&#x3066;  <pr>&#x308F;&#x308A;&#x3042;&#x3066;</pr></trd>,
    <trd>&#x914D;&#x5E03;  <pr>&#x306F;&#x3044;&#x3075;</pr></trd>,
    <trd>&#x914D;&#x9054;  <pr>&#x306F;&#x3044;&#x305F;&#x3064;</pr></trd>,
    <trd>&#x5206;&#x5E03;  <pr>&#x3076;&#x3093;&#x3077;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>distrib&uacute;cia</trd>,
    <trd>rozde&lcaron;ovanie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_z;&c_p;&c_o;&c_d;&c_ib;&c_l;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_p;&c_o;&c_v;&c_s;&c_ju;&c_d;&c_zh;&c_je;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>

