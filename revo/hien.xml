<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>hien</rad>/o
</kap>
<drv mrk="hien.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="hien.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Hyaena</trd>), nokta rabobesto el Afriko kaj Azio,
      sin nutranta per kadavra&jcirc;oj.
    </dif>
  </snc>
  <trd lng="de">Hy&auml;ne</trd>
  <trd lng="sv">hyena</trd>
</drv>
<drv mrk="hien.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="best.rabo0oj">rabobestoj</ref>
      (<trd lng="la">Hyaenidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Hy&auml;nen <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>
