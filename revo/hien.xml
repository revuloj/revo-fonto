<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hien.xml,v 1.28 2018/12/22 11:10:15 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>hien</rad>/o
</kap>
<drv mrk="hien.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Nokta <ref tip="lst" cel="best.rabo0o"
      lst="voko:rabobestoj">rabobesto</ref> el Afriko kaj Azio,
      sin nutranta per kadavra&jcirc;oj;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="hiened.0oj"><tld/>edoj</ref>
      (<trd lng="la">Hyaena</trd>):
      <ekz>
        mi a&ubreve;dis &ccirc;i tie la plendajn vo&ccirc;ojn de <tld/>oj kaj vidis la verdajn okulojn de
        lupoj, sed mi ne forkuradis
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        inter vi rondiras malbonaj homoj, kiel <tld/>oj en la &scirc;afejo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malbono, kiel <tld/>o, neniam iras unuope
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;akalo kaj <tld/>o donas sian vivon, por savi siajn idojn
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        anstata&ubreve; muziko sonis nokte &gcirc;emantaj blekoj de l' <tld/>oj, kaj de tempo al
        tempo la potenca vo&ccirc;o de la leono
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/5/53/Striped_hyena_(Hyaena_hyaena)_at_IGZoo_Park.JPG/640px-Striped_hyena_(Hyaena_hyaena)_at_IGZoo_Park.JPG"
      prm="CC BY-SA 3.0"
      >Strio<tld/>o (Hyaena hyaena), Besto&gcirc;ardeno Indira Gandhi, Visakhapatnam
        <fnt>
          <aut>Adityamadhav83, propra verko</aut>
          <lok><url ref="&WCW;/File:Striped_hyena_(Hyaena_hyaena)_at_IGZoo_Park.JPG"
              >https://commons.wikimedia.org/wiki/File:Striped_hyena_(Hyaena_hyaena)_at_IGZoo_Park.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_je;&c_n;&c_a;</trd>
  <trd lng="bg">&c_h;&c_i;&c_je;&c_n;&c_a;</trd>
  <trd lng="br">bleiz-broc'h</trd>
  <trd lng="ca">hiena</trd>
  <trd lng="cs">hyena</trd>
  <trd lng="de">Hy&auml;ne</trd>
  <trd lng="el">&ypsilon_ton;&alfa;&jota;&ny;&alfa;</trd>
  <trd lng="en">hyena</trd>
  <trd lng="es">hiena</trd>
  <trd lng="fa">&f_k;&a_f;&a_t;&a_A;&a_r;</trd>
  <trd lng="fr">hy&egrave;ne</trd>
  <trd lng="hu">hi&eacute;na</trd>
  <trd lng="id">hiena</trd>
  <trd lng="it">iena</trd>
  <trd lng="nl">hyena</trd>
  <trd lng="pl">hiena</trd>
  <trd lng="pt">hiena</trd>
  <trd lng="ru">&c_g;&c_i;&c_je;&c_n;&c_a;</trd>
  <trd lng="sk">hyena</trd>
  <trd lng="sv">hyena</trd>
</drv>
</art>
