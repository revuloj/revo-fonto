<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>poligon</rad>/o
</kap>
<drv mrk="poligo.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="poligo1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Polygonum</trd>).
    </dif>
    <bld lok="../bld/poligo.jpg">
      floraro de <tld/>o <klr>(Polygonum bistorta)</klr>
    </bld>
    <bld lok="../bld/poligo2.jpg">
      <tld/>o <klr>(Polygonum bistorta)</klr>
    </bld>
    <trd lng="de">Kn&ouml;terich</trd>
  </snc>
  <snc>
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="fagopi.0o">fagopiro</ref>.
    </dif>
  </snc>
  <snc>
    <uzo tip="fak">MAT</uzo>
    <dif>
      Plurlatero, plurangulo.
    </dif>
    <trdgrp lng="de">
      <trd>Polygon</trd>,
      <trd>Vieleck</trd>
    </trdgrp>
    <trd lng="tr">&ccedil;okgen</trd>
  </snc>
</drv>

</art>
