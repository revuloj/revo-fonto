<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>poligon</rad>/o
</kap>
<drv mrk="poligo.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="poligo.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Polygonum</trd>).
    </dif>
    <trd lng="de">Kn&ouml;terich</trd>
  </snc>
  <snc>
    <uzo tip="klr">(evitinda)</uzo>
    <dif>
      <ref tip="dif" cel="fagopi.0o">fagopiro</ref>.
    </dif>
  </snc>
  <snc>
    <uzo tip="fak">MAT</uzo>
    <dif>
      Plurlatero, plurangulo.
    </dif>
    <trdgrp lng="de">
      <trd>Polygon</trd>,
      <trd>Vieleck</trd>
    </trdgrp>
    <trd lng="tr">&ccedil;okgen</trd>
  </snc>
</drv>
<drv mrk="poligo.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Polygonaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Kn&ouml;terichgew&auml;chse</trd>
</drv>
</art>
