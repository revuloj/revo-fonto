<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>poligon</rad>/o
</kap>
<drv mrk="poligo.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="poligo1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Polygonum</trd>).
    </dif>
    <bld lok="../bld/poligo.jpg">
      floraro de <tld/>o <klr>(Polygonum bistorta)</klr>
    </bld>
    <bld lok="../bld/poligo2.jpg">
      <tld/>o <klr>(Polygonum bistorta)</klr>
    </bld>
    <trd lng="de">Kn&ouml;terich</trd>
    <trd lng="hu">keser&udblac;f&udblac;</trd>
    <trd lng="nl">veenwortel</trd>
    <trdgrp lng="ru">
      <trd>&c_g;&c_o;&c_r;&c_je;&c_c; <klr
tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>,
      <trd>&c_g;&c_r;&c_je;&c_ch;&c_i;&c_sh;&c_n;&c_i;&c_k;</trd>,
      <trd>&c_g;&c_r;&c_je;&c_ch;&c_i;&c_sh;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="fagopi.0o">fagopiro</ref>.
    </dif>
  </snc>
  <snc>
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 27</lok></fnt>
    <ref tip="dif" cel="later.plur0o">plurlatero.</ref>
    <trdgrp lng="de">
      <trd>Polygon</trd>,
      <trd>Vieleck</trd>
    </trdgrp>
    <trd lng="en">polygon</trd>
    <trd lng="fr">polygone</trd>
    <trdgrp lng="hu">
      <trd>soksz&ouml;g</trd>,
    <trd>poligon</trd></trdgrp>
    <trd lng="nl">veelhoek</trd>
    <trd
lng="ru">&c_m;&c_n;&c_o;&c_g;&c_o;&c_u;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_i;&c_k;
</trd>
    <trd lng="tr">&ccedil;okgen</trd>
  </snc>
</drv>
</art>
