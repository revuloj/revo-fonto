<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>graf</rad>/o
    </kap>

    <drv mrk="graf.0o">
      <kap>
	<ofc>*</ofc><tld/>o
      </kap>
      <snc mrk="graf.0o.nobela">
	<dif>
	  <ref tip="super" cel="nobel.0o">Nobelo</ref>
	  kies rango estas pli alta ol tiu barono kaj malpli alta ol
	  tiu de <ref cel="markiz.0o">markizo</ref>.
	</dif>
      </snc>
      <ref tip="hom" cel="graf1.0o">grafo<klr> (matematikaj sencoj)</klr></ref>
      <trd lng="de">Graf</trd>
      <trd lng="en">count</trd>
      <trd lng="es">conde</trd>
      <trd lng="fr">comte</trd>
      <trd lng="hu">gr&oacute;f</trd>
      <trd lng="nl">graaf</trd>
      <trd lng="ru">&c_g;&c_r;&c_a;&c_f;</trd>
      <trd lng="sv">greve</trd>
    </drv>

    <drv mrk="graf.0ujo">
      <kap>
	<tld/>ujo <fnt><bib>PIV2</bib></fnt>
      </kap>
      <snc>
	<dif>
	  Mezgranda administra unuo, kiel tiu estrata de <tld/>o.
	</dif>
      </snc>
      <ref tip="vid" cel="kanton.0o.usona">kantono</ref>
      <trd lng="en">county</trd>
      <trd lng="ru">&c_g;&c_r;&c_a;&c_f;&c_s;&c_t;&c_v;&c_o;</trd>
    </drv>

    <drv mrk="graf.0lando">
      <kap>
	<tld/>lando <fnt><bib>PIV2</bib></fnt>
      </kap>
      <snc mrk="graf.0lando.ujo">
	<ref tip="dif" cel="graf.0ujo"><tld/>ujo</ref>
      </snc>
      <snc mrk="graf.0lando.HIS">
	<uzo tip="fak">HIS</uzo>
	<dif>
	  Lando regata de land<tld/>o
	  (sendependi&gcirc;inta eks<tld/>ujo).
	</dif>
      </snc>
      <trd lng="ru">&c_l;&c_a;&c_n;&c_d;&c_g;&c_r;&c_a;&c_f;&c_s;&c_t;&c_v;&c_o;</trd>
    </drv>

    <drv mrk="graf.land0o">
      <kap>
	land<tld/>o <fnt>Z</fnt>
      </kap>
      <snc mrk="graf.lando0.HIS">
	<uzo tip="fak">HIS</uzo>
	<dif>
	  En la mezepoka Germanujo, <tld/>o kiu ne estas vasalo de
	  duko a&ubreve; princo.
	</dif>
      </snc>
      <trd lng="ru">&c_l;&c_a;&c_n;&c_d;&c_g;&c_r;&c_a;&c_f;</trd>
    </drv>

  </art>
