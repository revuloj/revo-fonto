<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: graf.xml,v 1.37 2023/11/11 11:36:36 revo Exp $">
    <kap><ofc>*</ofc>
      <rad>graf</rad>/o</kap>

    <drv mrk="graf.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="graf.0o.nobela">
        <dif>
          <ref tip="super" cel="nobel.0o">Nobelo</ref>
          kies rango estas pli alta ol tiu de <ref tip="vid" cel="baron.0o.nobela">barono</ref>
          kaj malpli alta ol tiu de <ref cel="markiz.0o">markizo</ref>.
        </dif>
      <ref tip="hom" cel="graf1.0o">grafo<klr> (matematikaj
      sencoj)</klr></ref>
      <trd lng="be">&c_g;&c_r;&c_a;&c_f;</trd>
      <trd lng="ca">comte</trd>
      <trd lng="de">Graf</trd>
      <trdgrp lng="en"><trd>count</trd>, <trd>earl</trd></trdgrp>
      <trd lng="es">conde</trd>
      <trd lng="fr">comte</trd>
      <trd lng="hu">gr&oacute;f</trd>
      <trd lng="it">conte</trd>
      <trd lng="nl">graaf</trd>
      <trdgrp lng="pl">
        <trd>hrabia</trd>,
        <trd>graf<klr> (w dawnych krajach Cesarstwa
        Niemieckiego)</klr></trd>,
        <trd>margrabia<klr> (w dawnych krajach Cesarstwa
        Niemieckiego)</klr></trd>,
        <trd>komes</trd>
      </trdgrp>
      <trd lng="pt">conde<klr tip="amb"> (t&iacute;tulo)</klr></trd>
      <trd lng="ru">&c_g;&c_r;&c_a;&c_f;</trd>
      <trd lng="sv">greve</trd>
      <trd lng="tr">kont</trd>
      </snc>
  <trdgrp lng="cs">
    <trd>diagram</trd>,
    <trd>graf</trd>,
    <trd>grafick&eacute; zn&aacute;zorn&ecaron;n&iacute;</trd>
  </trdgrp>
  <trd lng="ja">&#x4F2F;&#x7235;  <pr>&#x306F;&#x304F;&#x3057;&#x3083;&#x304F;</pr></trd>
  <trdgrp lng="sk">
    <trd>diagram</trd>,
    <trd>graf</trd>,
    <trd>krivka</trd>
  </trdgrp>
  <trd lng="uk">&c_g;&c_r;&c_a;&c_f;</trd>
</drv>

 <drv mrk="graf.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
    <snc>
      <dif>
         Rilata al grafo a&ubreve; grafino :
        <ekz>
           El la tri leteroj unu estis adresita: (...) la dua: al Lia
           Grafa Mo&scirc;to
           <fnt><bib>F</bib>, <lok>&FE; 42</lok></fnt>.
        </ekz>
      </dif>
    </snc>
    <trd lng="be">&c_g;&c_r;&c_a;&c_f;&c_s;&c_k;&c_ib;</trd>
   <trd lng="uk">&c_g;&c_r;&c_a;&c_f;&c_s;&#x44C;&#x43A;&c_i;&c_j;</trd>
</drv> 
    
    <drv mrk="graf.0ujo">
      <kap><tld/>ujo <fnt><bib>PIV2</bib></fnt></kap>
      <snc>
        <dif>
          Mezgranda administra unuo, kiel tiu estrata de <tld/>o:
          <ekz>
            la departemento, en kiu situas Mussidan, havas la nomon
            Dordonjo kaj anstata&ubreve;is la malnovan <tld/>ujon
            Perigordo
            <fnt>
              <vrk><url ref="http://esper.neuf.fr/esperante.html">Mussidan</url></vrk>.
            </fnt>
          </ekz>
        </dif>
      <ref tip="vid" cel="kanton.0o.usona">kantono</ref>
      <trd lng="be">&c_g;&c_r;&c_a;&c_f;&c_s;&c_t;&c_v;&c_a;</trd>
      <trd lng="ca">comtat</trd>
      <trd lng="de">Grafschaft</trd>
      <trdgrp lng="en"><trd>county</trd>, <trd>earldom</trd></trdgrp>
      <trd lng="es">condado</trd>
      <trd lng="fr">comt&eacute;</trd>
      <trdgrp lng="hu">
        <trd>gr&oacute;fs&aacute;g</trd>,
        <trd>megye</trd>
      </trdgrp>
      <trd lng="it">contea</trd>
      <trd lng="nl">graafschap</trd>
      <trd lng="pl">hrabstwo</trd>
      <trd lng="ru">&c_g;&c_r;&c_a;&c_f;&c_s;&c_t;&c_v;&c_o;</trd>
      </snc>
      <trd lng="zh">&#x53BF; <pr>xi&#xE0;n</pr></trd>
</drv>

<drv mrk="graf.0lando">
  <kap><tld/>lando <fnt><bib>PIV2</bib></fnt></kap>
  <snc mrk="graf.0lando.ujo">
    <ref tip="dif" cel="graf.0ujo"><tld/>ujo</ref>
  </snc>
  <snc mrk="graf.0lando.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Lando regata de land<tld/>o
      (sendependi&gcirc;inta eks<tld/>ujo).
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_f;&c_s;&c_t;&c_v;&c_a;</trd>
  <trd lng="ca">comtat</trd>
  <trd lng="en">landgraviate</trd>
  <trd lng="es">condado</trd>
  <trd lng="fr">comt&eacute;</trd>
  <trd lng="hu">tartom&aacute;ny</trd>
  <trd lng="ja">&#x4F2F;&#x7235;&#x9818;  <pr>&#x306F;&#x304F;&#x3057;&#x3083;&#x304F;&#x308A;&#x3087;&#x3046;</pr></trd>
  <trd lng="nl">graafschap</trd>
  <trd lng="pl">hrabstwo</trd>
  <trd lng="ru">&c_l;&c_a;&c_n;&c_d;&c_g;&c_r;&c_a;&c_f;&c_s;&c_t;&c_v;&c_o;</trd>
   <trd lng="tr">kontluk</trd>
  <trd lng="uk">&c_g;&c_r;&c_a;&c_f;&c_s;&c_t;&c_v;&#x43E;</trd>
  <trd lng="zh">&#x53BF; <pr>xi&#xE0;n</pr></trd>
</drv>

<drv mrk="graf.land0o">
  <kap>land<tld/>o <fnt>Z</fnt></kap>
  <snc mrk="graf.land0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      En la mezepoka Germanujo, <tld/>o kiu ne estas vasalo de
      duko a&ubreve; princo.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_n;&c_d;&c_g;&c_r;&c_a;&c_f;</trd>
  <trd lng="ca">landgravi</trd>
  <trd lng="en">landgrave</trd>
  <trd lng="es">condado</trd>
  <trd lng="fr">landgrave</trd>
  <trd lng="de">Landgraf</trd>
  <trd lng="hu">tartom&aacute;nygr&oacute;f</trd>
  <trd lng="ja">&#x65B9;&#x4F2F;  <pr>&#x307B;&#x3046;&#x306F;&#x304F;</pr></trd>
  <trd lng="nl">landgraaf</trd>
  <trdgrp lng="pl">
    <trd>landgraf<klr> (w dawnych krajach Cesarstwa
    Niemieckiego)</klr></trd>,
    <trd>landgrabia<klr> (w dawnych krajach Cesarstwa
    Niemieckiego)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_a;&c_n;&c_d;&c_g;&c_r;&c_a;&c_f;</trd>
  <trd lng="uk">&#x43B;&c_a;&#x43D;&c_d;&c_g;&c_r;&c_a;&c_f;</trd>
</drv>

  </art>
