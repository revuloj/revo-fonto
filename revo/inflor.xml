<art mrk="$Id: inflor.xml,v 1.27 2024/07/30 21:23:28 revo Exp $">
  <kap>
    <rad>infloresk</rad>/o
    <fnt><bib>PVEF</bib></fnt>
  </kap>

<drv mrk="inflor.0o">
  <kap><tld/>o</kap>
  <snc mrk="inflor.0o.BOT">
  <uzo tip="fak">BOT</uzo>
    <dif>
      &Ccirc;e <ref tip="malprt" cel="flor.0oplant">floroplantoj</ref>,
      la aran&gcirc;o de la <ref tip="prt" cel="flor.0o.BOT">floroj</ref>
      sur la tigo:
      <ekz>
        la instruado pri la naturo <klr>[&#8230;]</klr> konsistis precipe el la nomado de <tld/>oj kaj
        anteroj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Fantaziujo en Dan&gcirc;ero</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a trunko de agavo
        <fnt>
          <aut>Fidel Figueroa</aut>
          <vrk><url ref="&Monato;/2000/007033.php">Nepre &gcirc;ia nomo estas Meksiko</url></vrk>
          <bib>Monato</bib>
          <lok>2000/10, p. 14</lok>
          </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="flor.0aro.BOT">floraro</ref>
  </snc>
  <bld lok="../bld/infloresk.png">
    <tld/>oj
  </bld>
  <refgrp tip="sub">
    1. <ref cel="grapol.0o">grapolo</ref>,
    2. umbela grapolo,
    3. <ref cel="spik.0o">spiko</ref>,
    4. <ref cel="ament.0o">amento</ref>,
    5. <ref cel="spadik.0o">spadiko</ref>,
    6. kunmetita spiko,
    7. <ref cel="panikl.0o">paniklo</ref>,
    8. umbela paniklo,
    9. funela paniklo,
    10. <ref cel="umbel.0o">umbelo</ref>,
    11. kunmetita umbelo,
    12. du<ref cel="cum.0o">cumo</ref>,
    13. bula umbelo,
    14. <ref cel="kap.0o.BOT">kapo</ref>,
    15. <ref cel="kapitul.0o">kapitulo</ref>,
    16. <ref cel="strobi.0o">strobilo</ref>
  </refgrp>
  <trd lng="be">&c_s;&c_u;&c_k;&c_v;&c_je;&c_c;&c_mol;&c_c;&c_je;</trd>
  <trd lng="ca">infloresc&egrave;ncia</trd>
  <trd lng="de">Bl&uuml;tenstand</trd>
  <trd lng="es">inflorescencia</trd>
  <trd lng="fr">inflorescence</trd>
  <trd lng="hu">vir&aacute;gzat</trd>
  <trd lng="ja">&#33457;&#24207;  <pr>&#12363;&#12376;&#12423;</pr></trd>
  <trd lng="nl">bloeiwijze</trd>
  <trd lng="pt">infloresc&ecirc;ncia</trd>
  <trd lng="ru">&c_s;&c_o;&c_c;&c_v;&c_je;&c_t;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>inflorescencia</trd>,
    <trd>kvetenstvo</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>inflorescence</trd>,
    <trd>kv&ecaron;tenstv&iacute;</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_u;&c_c;&c_v;&c_ib;&c_t;&c_t;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#33457;&#19995; <pr>hu&amacron;c&oacute;ng</pr></trd>,
    <trd>&#33457;&#24207; <pr>hu&amacron;x&ugrave;</pr></trd>
  </trdgrp>
</drv>
