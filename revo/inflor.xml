<vortaro>
<art mrk="$Id: inflor.xml,v 1.20 2018/10/28 17:10:19 revo Exp $">
  <kap>
    <rad>infloresk</rad>/o
  </kap>

<drv mrk="inflor.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      &Ccirc;e <ref tip="malprt" cel="flor.0oplant">floroplantoj</ref>,
      la aran&gcirc;o de la <ref tip="prt" cel="flor.0o.BOT">floroj</ref>
      sur la tigo:
      <ekz>
        la instruado pri la naturo <klr>[&#8230;]</klr> konsistis precipe el la nomado de <tld/>oj kaj anteroj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Fantaziujo en Dan&gcirc;ero</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a trunko de agavo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007033.html"
          >Centra Ameriko: Nepre &gcirc;ia nomo estas Meksiko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="flor.0aro.BOT">floraro</ref>
  </snc>
  <bld lok="../bld/infloresk.png">
    <tld/>oj<klr>: 1. grapolo, 2. umbela grapolo, 3. spiko, 4.
    amento,
      5. spadiko, 6. kunmetita spiko, 7. paniklo, 8. umbela paniklo,
      9. funela paniklo, 10. umbelo, 11. kunmetita umbelo, 12.
      ducumo,
      13. bula umbelo, 14. kapo, 15. kapitulo, 16. strobilo.</klr>
  </bld>
  <trd lng="be">&c_s;&c_u;&c_k;&c_v;&c_je;&c_c;&c_mol;&c_c;&c_je;</trd>
  <trd lng="ca">infloresc&egrave;ncia</trd>
  <trd lng="de">Bl&uuml;tenstand</trd>
  <trd lng="es">inflorescencia</trd>
  <trd lng="fr">inflorescence</trd>
  <trd lng="hu">vir&aacute;gzat</trd>
  <trd lng="nl">bloeiwijze</trd>
  <trd lng="pt">infloresc&ecirc;ncia</trd>
  <trd lng="ru">&c_s;&c_o;&c_c;&c_v;&c_je;&c_t;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>inflorescencia</trd>,
    <trd>kvetenstvo</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>inflorescence</trd>,
    <trd>kv&ecaron;tenstv&iacute;</trd>
  </trdgrp>
</drv>
  </art>
