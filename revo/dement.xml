<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>dement</rad>/i
</kap>
<drv mrk="dement.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <ref tip="dif" cel="konfir.mal0i">malkonfirmi</ref>
    <ekz>
      la registaro <tld/>is mensogajn komunikojn de eksterlandaj
      &jcirc;urnaloj
      <fnt><bib>PV</bib></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_v;&c_ja;&c_r;&c_g;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">desmentir</trd>
  <trdgrp lng="cs">
    <trd>dementovat</trd>,
    <trd>popírat</trd>,
    <trd>vyvracet</trd>
  </trdgrp>
  <trd lng="es">desmentir</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;mentir</trd>,
    <trd>contester<klr tip="ind"> (d&eacute;mentir)</klr></trd>,
    <trd>infirmer</trd>,
    <trd>r&eacute;cuser<klr tip="ind"> (d&eacute;mentir)</klr></trd>,
    <trd>r&eacute;futer<klr tip="ind"> (d&eacute;mentir)</klr></trd>
  </trdgrp>
  <trd lng="hu">c&aacute;fol</trd>
  <trdgrp lng="pl">
    <trd>dementowa&cacute;</trd>,
    <trd>sprostowa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">desmentir</trd>
  <trd
  lng="ru">&c_o;&c_p;&c_r;&c_o;&c_v;&c_je;&c_r;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>dementova&tcaron;</trd>,
    <trd>popierať</trd>,
    <trd>vyvracať</trd>
  </trdgrp>
</drv>
</art>
