<kap>
  <rad>ekzantem</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ekzant.0o">
  <kap><tld/>o</kap>
  <snc mrk="ekzant.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Ru&gcirc;aj
      <ref tip="super" cel="makul.hauxt0o.MED">ha&ubreve;tmakuloj</ref>,
      kiuj aperas &ccirc;e diversaj infektaj malsanoj:
      <ekz>
        tifa <tld/>o;
      </ekz>
      <ekz>
        la dermatologo esploris la <tld/>on sur mia mano
        <fnt>
          <aut>I. Cesaro, trad. G. Gehlert, A. Gr&ouml;tzner, S. Herrgesell
          kaj B. Lorenz</aut>,
          <vrk><url ref="http://www.esperanto-bayern.com/Kronach/aleo31.htm">
          Stutgarta Aleo 31</url></vrk>,
          <lok>[2009]</lok>
        </fnt>
      </ekz>
      </dif>
    <refgrp tip="vid">
      <ref cel="ekzem.0o">ekzemo</ref>,
      <ref cel="erupci.0o">erupcio</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_y;&c_p;</trd>,
    <trd>&c_e;&c_k;&c_z;&c_a;&c_n;&c_t;&c_e;&c_m;&c_a;</trd> 
  </trdgrp>
  <trd lng="ca">exantema</trd>
  <trd lng="cs">vyr&aacute;&zcaron;ka</trd>
  <trdgrp lng="de">
     <trd>Ausschlag</trd>,
     <trd>Hautausschlag</trd>,
     <trd>Exanthem<klr> [fak]</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>exanthema</trd>,
     <trd>rash</trd>
  </trdgrp>
  <trd lng="es">exantema</trd>
  <trd lng="fr">exanth&egrave;me</trd>
  <trd lng="hu">b&odblac;rki&uuml;t&eacute;s</trd>
  <trdgrp lng="it">
     <trd>esantema</trd>,
     <trd>rash</trd>
  </trdgrp>
  <trd lng="ja">&#x767A;&#x75B9;  <pr>&#x306F;&#x3063;&#x3057;&#x3093;</pr></trd>
  <trd lng="nl">exantheem</trd>
  <trd lng="pl">wykwit</trd>
  <trd lng="pt">exantema</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_y;&c_p;&c_mol;</trd>,
    <trd>&c_e;&c_k;&c_z;&c_a;&c_n;&c_t;&c_je;&c_m;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">vyr&aacute;&zcaron;ka</trd>
  <trdgrp lng="uk">
    <trd>&#x435;&#x43A;&#x437;&#x430;&#x43D;&#x442;&#x435;&#x43C;&#x430;</trd>,
    <trd>&#x432;&#x438;&#x441;&#x438;&#x43F;</trd>
  </trdgrp>
  <trd lng="zh">&#x75B9; <pr>zh&#x11B;n</pr></trd>
</drv>
