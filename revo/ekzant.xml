<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>ekzantem</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="ekzant.0o">
<kap><tld/>o</kap>
<uzo tip="fak">MED</uzo>
  <snc>
  <dif>
  Ru&gcirc;aj makuloj sur la ha&ubreve;to, kiuj aperas &ccirc;e
diversaj infektaj malsanoj:
  <ekz>tifa <tld/>o.</ekz>
  </dif>
  <refgrp tip="vid">
  <ref cel="ekzem.0o">ekzemo</ref>,
  <ref cel="erupci.0o">erupcio</ref>.
  </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_y;&c_p;</trd>,
    <trd>&c_e;&c_k;&c_z;&c_a;&c_n;&c_t;&c_e;&c_m;&c_a;</trd> 
  </trdgrp>
  <trdgrp lng="de">
     <trd>Ausschlag</trd>,
     <trd>Hautausschlag</trd>,
     <trd>Exanthem<klr> [fak]</klr></trd>
  </trdgrp>
  <trd lng="es">exantema</trd>
  <trd lng="fr">exanth&egrave;me</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_y;&c_p;&c_mol;</trd>,
    <trd>&c_e;&c_k;&c_z;&c_a;&c_n;&c_t;&c_je;&c_m;&c_a;</trd>
  </trdgrp>
</drv>
</art>
