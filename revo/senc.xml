<drv mrk="senc.iu0e">
  <kap>iu<tld/>e, <var><kap>certa<tld/>e</kap></var></kap>
  <snc mrk="senc.iu0e.ARG">
    <dif>
      <ref tip="super" cel="iel.0">Iel</ref> rigardate, la&ubreve; iu
      vidpunkto:
      <ekz>
        son&gcirc;oj estas parto de nia &ccirc;iutaga vivo <klr>[&#8230;]</klr> iu<tld/>e gravaj
        <fnt>
          <aut>Reza Kheir-Khah</aut>
            <vrk><url ref="&Monato;/2000/007087.php">Mirinda dimensio de nia vivo</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 16</lok>
          </fnt>;
      </ekz><ekz>
        iu<tld/>e animpuriga muziko
        <fnt>
          <aut>J. Zajdman, P. Mo&jcirc;ajev</aut>,
          <vrk>Ege ri&ccirc;aj harmonie</vrk>,
          <bib>Monato</bib>,
          <lok>2002/11, p. 24a</lok>
