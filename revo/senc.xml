<drv mrk="senc.plen0a">
  <kap>plen<tld/>a</kap>
  <snc mrk="senc.plen0a.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      <klr>(pri vorto:)</klr> Havanta propran leksikan signifon,
      respondan al la realo en ties a&jcirc;oj, agoj a&ubreve; ecoj, kontraste al
      gramatikiloj perdintaj sian semantikan valoron kaj plenumantaj
      funkciojn sintaksajn:
      <ekz>
        <ind>plen<tld/>a verbo</ind>;
        <trd lng="en">lexical verb</trd>
        <trd lng="de">Vollverb</trd>
        <trdgrp lng="ru">
          <trd>&c_z;&c_n;&c_a;&c_m;&c_je;&c_n;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_g;&c_l;&c_a;&c_g;&c_o;&c_l;</trd>,
          <trd>&c_p;&c_o;&c_l;&c_n;&c_o;&c_z;&c_n;&c_a;&c_ch;&c_n;&c_y;&c_j; &c_g;&c_l;&c_a;&c_g;&c_o;&c_l;</trd>
        </trdgrp>
      </ekz><ekz>
        <klr>(la verbo &#8249;aspekti&#x203A;)</klr> &ccirc;iam restas en la limregiono inter
        kopulaj kaj plen<tld/>aj verboj
        <fnt><aut>H.A. de Hoog</aut>: <vrk>Du malsanoj en Esperanto</vrk>.
        <lok>INKO, ISBN 91-7303-136-4. &para;53</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>autosemantisch</trd>,
      <trd>haupts&auml;chlich (Wort)</trd>,
      <trd>selbstst&auml;ndig (Wort)</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>content (word)</trd>
      <trd>full<klr> word</klr></trd>,
      <trd>autonomous<klr> (word)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_n;&c_a;&c_m;&c_je;&c_n;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_p;&c_o;&c_l;&c_n;&c_o;&c_z;&c_n;&c_a;&c_ch;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
<trd lng="be">&c_p;&c_a;&c_w;&c_n;&c_a;&c_z;&c_n;&c_a;&c_ch;&c_n;&c_y;</trd>
</drv>

<drv mrk="senc.plur0a">
