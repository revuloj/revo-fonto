<drv mrk="senc.sam0eco">
  <kap>sam<tld/>eco</kap>
  <snc mrk="senc.sam0eco.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="dif" cel="sinoni.0eco.LIN">Sinonimeco</ref>:
      <ekz>
        internacia simileco kaj sam<tld/>eco
        de trafiksignoj gravas en internacia trafiko
          <fnt>
            <aut>P. Aitai</aut>,
            <vrk><url ref="http://peeteraitai.blogspot.com/2006_12_01_archive.html">
            blogo</url></vrk>,
            <lok>2006-12-30</lok>
