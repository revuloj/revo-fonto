<drv mrk="senc.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="senc.sen0a.propre">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Havanta nenian <tld/>on; <ref tip="vid" cel="absurd.0a">absurda</ref>:
      <ekz>
        sen<tld/>a parolado, <klr>[&#8230;]</klr> tro libera babilado
        <fnt>
          <bib>NT</bib>
          <lok>&Efe; 5:4</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>senseless</trd>,
      <trd>nonsensical</trd>
    </trdgrp>
    <trd lng="es">sin sentido</trd>
    <trd lng="he">&het;&samekh;&resh;
    &mem;&shin;&mem;&ayin;&vav;&tav;</trd>
    <trdgrp lng="hu">
      <trd>&eacute;rtelmetlen</trd>,
      <trd>k&eacute;ptelen</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>bezsensowny</trd>,
      <trd>absurdalny</trd>,
      <trd>nonsensowny</trd>
    </trdgrp>
  </snc>
  <snc mrk="senc.sen0a.malsagxa">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Malsa&gcirc;a, <ref tip="vid" cel="frenez.0a.sensenca">freneza<sncref/></ref>:
