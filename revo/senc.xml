<drv mrk="senc.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="senc.sen0a.propre">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Havanta nenian <tld/>on:
      <ekz>
        sen<tld/>a parolado, <klr>[&#8230;]</klr> tro libera babilado
        <fnt>
          <bib>NT</bib>
          <lok>Efesanoj 5:4</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>senseless</trd>,
      <trd>nonsensical</trd>
    </trdgrp>
    <trd lng="es">sin sentido</trd>
    <trd lng="he">&het;&samekh;&resh;
    &mem;&shin;&mem;&ayin;&vav;&tav;</trd>
    <trdgrp lng="hu">
      <trd>&eacute;rtelmetlen</trd>,
      <trd>k&eacute;ptelen</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>bezsensowny</trd>,
      <trd>absurdalny</trd>,
      <trd>nonsensowny</trd>
    </trdgrp>
  </snc>
  <snc mrk="senc.sen0a.malsagxa">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Malsa&gcirc;a:
      <ekz>
        la petoj, kiujn tiu virino sendis al la &ccirc;ielo, estis sen<tld/>aj, maljustaj, tro
        postulemaj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        pro ne&gcirc;usta timo anta&ubreve; sen<tld/>aj atakantoj ili penas <klr>[&#8230;]</klr> nei
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Dua Kongreso Esperantista en Gen&egrave;ve en la 28a de a&ubreve;gusto 1906</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Trajan</nom> konvinki&gcirc;is, ke &ccirc;io estas sen<tld/>a kaj
        freneza
        <fnt>
          <aut>J. Modest</aut>,
          <vrk>La ikono</vrk>,
          <bib>Monato</bib>,
          <lok>2001:12, p. 23a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>senseless</trd>,
      <trd>absurd</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>sinsentido</trd>,
      <trd>absurdo</trd>
    </trdgrp>
    <trd lng="he">&het;&samekh;&resh; &he;&#8503;&yod;&vav;&fnun;</trd>
    <trdgrp lng="hu">
      <trd>ostoba</trd>,
      <trd>abszurd</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>durny</trd>,
      <trd>g&lstroke;upi</trd>,
      <trd>bzdurny</trd>,
      <trd>chory</trd>,
      <trd>poroniony</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_s;&c_s;&c_e;&c_n;&c_s;&c_o;&c_w;&c_n;&c_y;</trd>
  <trd lng="bg">&c_b;&c_je;&c_z;&c_s;&c_m;&c_i;&c_s;&c_l;&c_je;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>absurdn&iacute;</trd>,
    <trd>nesmysln&yacute;</trd>
  </trdgrp>
  <trd lng="de">sinnlos</trd>
  <trd lng="es">absurdo</trd>
  <trd lng="fr">absurde</trd>
  <trdgrp lng="it">
    <trd>insensato</trd>,
    <trd>assurdo</trd>
    </trdgrp>
  <trd lng="nl">zinloos</trd>
  <trd lng="ru">&c_b;&c_je;&c_s;&c_s;&c_m;&c_y;&c_s;&c_l;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>absurdn&yacute;</trd>,
    <trd>nezmyseln&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="senc.sen0ajxo">
  <kap><ofc>*</ofc>sen<tld/>a&jcirc;o</kap>
  <snc mrk="senc.sen0ajxo.aserto">
    <dif>
      Aserto, okaza&jcirc;o, situacio, kiu &scirc;ajne ne havas
      <tld/>on, ne estas komprenebla:
      <ekz>
        vi parolas sen<tld/>a&jcirc;on, mia amiko
        <fnt>
          <bib>F</bib>
          <lok>&FE; 35</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bu&scirc;o de malsa&gcirc;uloj elparolas sen<tld/>a&jcirc;on
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 15:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tuta afero estis nenio krom sen<tld/>a&jcirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>!
      </ekz>
      <ekz>
        <klr>[ili]</klr> okupas sin je &ccirc;ia plej sencela kaj senutila sen<tld/>a&jcirc;o, se &gcirc;i
        nur estas en modo kaj konforma al la rutinaj ideoj de la amaso
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        devigi min sekvi arbitrajn sen<tld/>a&jcirc;ojn
        <fnt><bib>BonaLingvo</bib>, <lok>p. 29a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_g;&c_l;&c_u;&c_p;&c_o;&c_s;&c_t;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_s;&c_m;&c_i;&c_s;&c_l;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Unsinn</trd>
  <trdgrp lng="en">
    <trd>nonsense</trd>,
    <trd>senselessness</trd>,
    <trd>absurdity</trd>
  </trdgrp>
  <trdgrp lng="es">
  	<trd>sinsentido</trd>,
	<trd>absurdo</trd>
  </trdgrp>
  <trd lng="fr">absurdit&eacute;</trd>
  <trd lng="he">&shin;&tet;&vav;&tav;</trd>
  <trdgrp lng="hu">
    <trd>&eacute;rtelmetlens&eacute;g</trd>,
    <trd>k&eacute;ptelens&eacute;g</trd>
    <trd>ostobas&aacute;g</trd>
  </trdgrp>
  <trd lng="it">assurdit&agrave;</trd>
  <trdgrp lng="pl">
    <trd>nonsens</trd>,
    <trd>absurd</trd>,
    <trd>andron</trd>,
    <trd>banialuka</trd>,
    <trd>bzdura</trd>,
    <trd>g&lstroke;upota</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_s;&c_s;&c_m;&c_y;&c_s;&c_l;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_z;&c_d;&c_o;&c_r;</trd>,
    <trd>&c_n;&c_je;&c_l;&c_je;&c_p;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
