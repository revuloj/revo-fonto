<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gnet.xml,v 1.34 2025/08/24 16:34:48 revo Exp $">
<kap>
  <rad>gnet</rad>/o
<fnt><bib>PVEF</bib></fnt></kap>
<drv mrk="gnet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio
      <ref tip="malprt" cel="gnetac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Gnetum</trd>) de tropikaj grimpplantoj kaj
      arbetoj el
      <ref tip="lst" cel="konifer.0o" lst="voko:koniferoj">koniferoj</ref>
      kun veraj folioj, kulturataj pro la man&gcirc;eblaj semoj
      kaj folioj:
      <ekz>
        <tld/>o estas genro de ligni&gcirc;antaj plantoj kun 30 specioj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=279415">Gneto</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
    lok="&WCU;/thumb/a/a0/Gnetum_gnemon_BotGardBln1105B.jpg/640px-Gnetum_gnemon_BotGardBln1105B.jpg"
    prm="CC-BY-SA-3.0">bran&ccirc;oj de <tld/>o (Gnetum gnemon)
        <fnt>
          <lok><url
          ref="&WCW;/File:Gnetum_gnemon_BotGardBln1105B.jpg">&WCW;/File:Gnetum_gnemon_BotGardBln1105B.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_g;&c_n;&c_e;&c_t;&c_u;&c_m;</trd>
  <trd lng="ca">g&egrave;nere Gnetum</trd>
  <trd lng="cs">chvojn&iacute;k</trd>
  <trd lng="de">Gnetum</trd>
  <trd lng="en">Gnetum</trd>
  <trd lng="fr">gnetum</trd>
  <trd lng="hu">gn&eacute;tum</trd>
  <trd lng="id">melinjo</trd>
  <trd lng="ja">&#12464;&#12493;&#12484;&#12512;</trd>
  <trd lng="pl">gniot <klr>(ro&sacute;lina)</klr></trd>
  <trd lng="pt">gneto</trd>
  <trd lng="ru">&c_g;&c_n;&c_je;&c_t;&c_u;&c_m;</trd>
  <trd lng="sk">chvojn&iacute;k</trd>
  <trd lng="uk">&c_g;&c_n;&c_je;&c_t;&c_u;&c_m;</trd>
  <trd lng="zh">&#20080;&#40635;&#34276;&#31185; <pr>m&#462;im&aacute;t&eacute;ngk&emacron;</pr></trd>
</drv>

<drv mrk="gnet.0oplantoj">
  <kap><tld/>oplantoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:botanikaj_ordoj">Ordo</ref> el la klaso <ref
      tip="malprt" cel="konifer.0oj">koniferoj</ref>
      (<trd lng="la">Gnetales</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_n;&c_e;&c_t;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_k;&c_l;&c_ja;&c_s;)</klr></trd>
  <trd lng="ca">gnetofitins</trd>
  <trd lng="de">Gnetales</trd>
  <trdgrp lng="en">
    <trd>Gnetales</trd>,
    <trd>Gnetophyta</trd>
  </trdgrp>
  <trd lng="fr">gnetophytes</trd>
  <trd lng="pl">gniotowce</trd>
  <trd lng="ru">&c_g;&c_n;&c_je;&c_t;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_o;&c_t;&c_d;&c_je;&c_l;)</klr></trd>
  <trd lng="zh">&#20080;&#40635;&#34276;&#30446; <pr>m&#462;im&aacute;t&eacute;ngm&ugrave;</pr></trd>
</drv>
</art>
