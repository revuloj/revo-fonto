<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>gnet</rad>/o
</kap>
<drv mrk="gnet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="gent.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Gnetum</trd>), gimnospermaj tropikaj grimpplantoj kaj arbetoj
      kun veraj folioj, kulturata pro la man&gcirc;eblaj semoj kaj folioj.
    </dif>
  </snc>
</drv>
<drv mrk="gnet.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="gimnos.0oj">gimnospermoj</ref>
      (<trd lng="la">Gnetaceae</trd>) kun multaj ecoj de angiospermoj.
    </dif>
  </snc>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
