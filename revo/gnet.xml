<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>gnet</rad>/o
</kap>
<drv mrk="gnet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="gnet.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Gnetum</trd>) de tropikaj grimpplantoj kaj
      arbetoj kun veraj folioj, kulturataj pro la man&gcirc;eblaj semoj
      kaj folioj.
    </dif>
  </snc>
</drv>
<drv mrk="gnet.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="gnet.0oplantoj"><tld/>oplantoj</ref>
      (<trd lng="la">Gnetaceae</trd>).
    </dif>
  </snc>
</drv>
<drv mrk="gnet.0oplantoj">
  <kap><tld/>oplantoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Divido de la <ref tip="super" cel="plant.0oj">plantoj</ref>
      (<trd lng="la">Gnetophyta</trd>).
    </dif>
  </snc>
</drv>
</art>
