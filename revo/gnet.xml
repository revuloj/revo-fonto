<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gnet</rad>/o
</kap>
<drv mrk="gnet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="gnetac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Gnetum</trd>) de tropikaj grimpplantoj kaj
      arbetoj kun veraj folioj, kulturataj pro la man&gcirc;eblaj semoj
      kaj folioj.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_n;&c_e;&c_t;&c_u;&c_m;</trd>
  <trd lng="ca">g&egrave;nere Gnetum</trd>
  <trd lng="de">Gnetum</trd>
  <trd lng="fr">gnetum</trd>
  <trd lng="hu">gn&eacute;tum</trd>
  <trd lng="pt">gneto</trd>
  <trd lng="ru">&c_g;&c_n;&c_je;&c_t;&c_u;&c_m;</trd>
</drv>

<drv mrk="gnet.0oplantoj">
  <kap><tld/>oplantoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO"
      lst="voko:botanikaj_ordoj">Ordo</ref> el la klaso <ref
      tip="malprt"
      cel="konifer.0oj">koniferoj</ref>
      (<trd lng="la">Gnetales</trd>).
    </dif>
  </snc>
  <trd lng="ca">gnetofitins</trd>
  <trd lng="de">Gnetales</trd> 
  <trd lng="fr">gnetophytes</trd>
  <trd lng="ru">&c_g;&c_n;&c_je;&c_t;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_o;&c_t;&c_d;&c_je;&c_l;)</klr></trd>
</drv>
</art>
