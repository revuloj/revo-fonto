<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>gnet</rad>/o
</kap>
<drv mrk="gnet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="gnetac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Gnetum</trd>) de tropikaj grimpplantoj kaj
      arbetoj kun veraj folioj, kulturataj pro la man&gcirc;eblaj semoj
      kaj folioj.
    </dif>
  </snc>
  <trd lng="de">Gnetum</trd>
  <trd lng="fr">gnetum</trd>
</drv>

<drv mrk="gnet.0oplantoj">
  <kap><tld/>oplantoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Divido de la <ref tip="super" cel="plant.0oj">plantoj</ref>
      (<trd lng="la">Gnetophyta</trd>).
    </dif>
  </snc>
  <trd lng="fr">gnetophytes</trd>
</drv>
</art>
