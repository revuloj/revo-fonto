<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: med.xml,v 1.16 2024/01/20 12:39:18 revo Exp $">
<kap>
  <rad>med</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="med.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      Lo&gcirc;anto de <ref tip="vid" cel="med.0ujo"><tld lit="M"/>ujo</ref>:
      <ekz>
        la re&gcirc;o de Asirio transkondukis la Izraelidojn en Asirion, kaj lo&gcirc;igis ilin
        <klr>[&#8230;]</klr> en la urboj de la <tld lit="M"/>oj
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 18:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        via regno estas dividita kaj donita al la <tld lit="M"/>oj kaj Persoj
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 5:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>oj, kiuj batalas forkurante
        <fnt><bib>DonKihxoto</bib>
        <lok>&ccirc;apitro 18a, p. 127a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="med1.0o">medo (mielvino)</ref>
  <trd lng="be">&c_m;&c_ib;&c_d;&c_y;&c_je;&c_c;</trd>
  <trd lng="ca">mede<klr> (subst.)</klr></trd>
  <trd lng="cs">medovina</trd>
  <trd lng="de">Meder</trd>
  <trd lng="fr">M&egrave;de<klr> (subst.)</klr></trd>
  <trd lng="hu">m&eacute;d</trd>
  <trd lng="ja">&#34562;&#x871C;&#37202;  <pr>&#x306F;&#12385;&#x307F;&#12388;&#12375;&#12421;</pr></trd>
  <trd lng="pl">Med</trd>
  <trd lng="pt">medo (&eacute;) <klr tip="amb">(povo da M&eacute;dia)</klr></trd>
  <trd lng="ro">Med</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_i;&c_d;&c_ja;&c_n;&c_i;&c_n;</trd>,
    <trd>&c_m;&c_i;&c_d;&c_i;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="sk">medovina</trd>
  <trd lng="uk">&#x43C;&c_je;&c_d; (&#x43D;&c_a;&#x43F;&c_ib;&c_j;)</trd>
</drv>

<drv mrk="med.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>oj:
      <ekz>
        oni trovis en <nom>A&hcirc;meta</nom>, la re&gcirc;a kastelo en la
        <tld lit="M"/>a lando, unu skribrula&jcirc;on, en kiu
        estis skribita la sekvanta pormemora&jcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Ezra 6:2</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_d;&c_y;&c_j;&c_s;&c_k;&c_ib;</trd>
  <trd lng="ca">mede<klr> (adj.)</klr></trd>
  <trd lng="de">medisch</trd>
  <trd lng="fr">m&egrave;de<klr> (adj.)</klr></trd>
  <trd lng="hu">m&eacute;d</trd>
  <trd lng="pl">medyjski</trd>
  <trdgrp lng="pt">
    <trd>medo (&eacute;) <klr tip="amb">(adj., relativo aos medos</klr></trd>,
    <trd>m&eacute;dico <klr tip="amb">(adj., relativo aos medos</klr></trd>
  </trdgrp>
  <trd lng="ro">Median</trd>
  <trd lng="ru">&c_m;&c_i;&c_d;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="med.0ujo">
  <kap><tld lit="M"/>ujo</kap>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      Inter la 7a jarcento a. K. kaj 550 a. K.
      konfederacio de pluraj persaj kaj asiriaj provincoj en okcidenta Azio,
      sude de la Kaspia Maro; nun regiono en nordokcidenta Irano:
      <ekz>
        armu <klr>[&#8230;]</klr> la re&gcirc;ojn de <tld lit="M"/>ujo, &gcirc;iajn komandantojn
        kaj &ccirc;iujn &gcirc;iajn estrojn
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 51:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        la le&gcirc;oj de <tld lit="M"/>ujo kaj Persujo &ccirc;iu malpermeso a&ubreve; ordono,
        konfirmita de la re&gcirc;o, devas resti ne&scirc;an&gcirc;ebla
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 6:15</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="medi1.0o">Medio</ref>
  </snc>
  <trd lng="be">&c_M;&c_ib;&c_d;&c_y;&c_ja;</trd>
  <trd lng="ca">M&egrave;dia</trd>
  <trd lng="de">Medien <klr>(Altertum)</klr></trd>
  <trd lng="fr">M&eacute;die</trd>
  <trd lng="hu">M&eacute;dia</trd>
  <trd lng="pl">Media <klr>(kraina)</klr></trd>
  <trd lng="pt">M&eacute;dia</trd>
  <trd lng="ru">&c_M;&c_i;&c_d;&c_i;&c_ja;</trd>
</drv>
</art>

