<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: med.xml,v 1.9 2020/12/30 22:10:32 revo Exp $">
<kap>
  <rad>med</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="med.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      Ano de popolo vivinta en <ref tip="vid"
      cel="med.0ujo"><tld/>ujo</ref> ekde la 2a jarmilo a. K.,
      konkerinta
      en la 7a jarcento a. K. Persujon kaj Asirion, fondante imperion,
      kiu
      ruini&gcirc;is &ccirc;irka&ubreve; 550 a. K. pro persa invado:
      <ekz>
        la <tld/>oj, kiuj batalas forkurante
        <fnt><bib>DonKihxoto</bib>
        <lok>&ccirc;apitro 18a, p. 127a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">mede<klr> (subst.)</klr></trd>
  <trd lng="cs">medovina</trd>
  <trd lng="de">Meder</trd>
  <trd lng="fr">M&egrave;de<klr> (subst.)</klr></trd>
  <trd lng="hu">m&eacute;d</trd>
  <trd lng="pl">Med</trd>
  <trd lng="pt">medo (&eacute;) <klr tip="amb">(povo da M&eacute;dia)</klr></trd>
  <trd lng="ro">Med</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_i;&c_d;&c_ja;&c_n;&c_i;&c_n;</trd>,
    <trd>&c_m;&c_i;&c_d;&c_i;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="sk">medovina</trd>
</drv>

<drv mrk="med.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>oj.
    </dif>
  </snc>
  <trd lng="ca">mede<klr> (adj.)</klr></trd>
  <trd lng="de">medisch</trd>
  <trd lng="fr">m&egrave;de<klr> (adj.)</klr></trd>
  <trd lng="hu">m&eacute;d</trd>
  <trd lng="pl">medyjski</trd>
  <trdgrp lng="pt">
    <trd>medo (&eacute;) <klr tip="amb">(adj., relativo aos medos</klr></trd>,
    <trd>m&eacute;dico <klr tip="amb">(adj., relativo aos medos</klr></trd>
  </trdgrp>
  <trd lng="ro">Median</trd>
  <trd lng="ru">&c_m;&c_i;&c_d;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="med.0ujo">
  <kap><tld lit="M"/>ujo</kap>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      La lando origine lo&gcirc;ata de la <ref tip="vid"
      cel="med.0o"><tld/>oj</ref>, en okcidenta Azio,
      sude de la Kaspia Maro, nuna nordokcidenta Irano.
    </dif>
    <ref tip="sin" cel="medi1.0o">Medio</ref>
  </snc>
  <trd lng="ca">M&egrave;dia</trd>
  <trd lng="de">Medien</trd>
  <trd lng="fr">M&eacute;die</trd>
  <trd lng="hu">M&eacute;dia</trd>
  <trd lng="pl">Media <klr>(kraina)</klr></trd>
  <trd lng="pt">M&eacute;dia</trd>
  <trd lng="ru">&c_M;&c_i;&c_d;&c_i;&c_ja;</trd>
</drv>
</art>

