<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fam</rad>/o
</kap>
<drv mrk="fam.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Publikaj, rondirantaj, neprecizaj diroj; onidiroj<sup>Z</sup>:
      <ekz>kuras la <tld/>o ke<fnt>Z</fnt>;</ekz>
      <ekz>kurigi<fnt>Z</fnt>,</ekz>
      <ekz>disvastigi <tld/>on<fnt>Z</fnt>;</ekz>
      <ekz>tiu <tld/>o trakuras la urbon
      (<ref tip="sin" cel="flug.0i">flugi</ref>);
      </ekz>
      <ekz>mi eksciis la&ubreve; la <tld/>diroj, ke...</ekz>
      <ref tip="sin" cel="klacx.0o">kla&ccirc;o</ref>.
    </dif>
    <trdgrp lng="de">
     <trd>Fama</trd>,
     <trd>Ger&uuml;cht</trd></trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_l;&c_u;&c_h;&c_i;</trd>,
      <trd>&c_m;&c_o;&c_l;&c_v;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <dif>
      Granda diskonateco &ccirc;e la publiko pri iu a&ubreve; io:
      <ekz>havi bonan, malbonan <tld/>on.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="renom.0o">renomo</ref>,
      <ref cel="reputa.0o">reputacio</ref>,
      <ref cel="glor.0o">gloro</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="trumpe.0i">trumpeti</ref>,
      <ref cel="reklam">reklami</ref>.
    </refgrp>
    <trdgrp lng="de">
     <trd>Ruf</trd>,
     <trd>Leumund</trd>,
     <trd>Ruhm</trd></trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_l;&c_a;&c_v;&c_a;</trd>,
      <trd>&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
    </trdgrp>
   </snc>
   <trd lng="en">rumor</trd>
</drv>
<drv mrk="fam.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Tre konata:
      <ekz><tld/>a a&ubreve;toro;</ekz>
      <ekz><tld/>ulo<fnt>Z</fnt>;</ekz>
      <ekz>lar&gcirc;e <tld/>a gloro<fnt>Z</fnt>;</ekz>
      <ekz><tld/>e konata scienculo, elpensa&jcirc;o,</ekz>
    </dif>
  </snc>
  <trd lng="cs">slavn&yacute;</trd>
  <trdgrp lng="de">
     <trd>namhaft</trd>,
     <trd>ber&uuml;hmt</trd></trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_z;&c_n;&c_a;&c_m;&c_je;&c_n;&c_i;&c_t;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
<drv mrk="fam.0eco">
  <kap><tld/>eco<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Vasta konateco.
    </dif>
  </snc>
  <trd lng="ru">&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>
<drv mrk="fam.0igi">
  <kap>(dis)<tld/>igi, <tld/>ecigi</kap>
  <snc>
    <dif>
      Vaste konatigi.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>bekannt machen</trd>,
     <trd>ber&uuml;hmt machen</trd></trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_s;&c_l;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_l;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="fam.0emulo">
  <kap><tld/>emulo<fnt>Z</fnt></kap>
  <snc>
    <dif>
      <tld lit="F"/>avida homo. 
    </dif>
  </snc>
  <trd lng="ru">&c_ch;&c_je;&c_s;&c_t;&c_o;&c_l;&c_ju;&c_b;&c_je;&c_c;</trd>
</drv>
<drv mrk="fam.mis0igi">
  <kap>mis<tld/>igi</kap>
  <snc>
    <dif>
      Malfavore diskonatigi 
    </dif>
    <refgrp tip="sin">
      <ref cel="nigr.0igi">nigrigi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>schlechtmachen</trd>,
     <trd>anschw&auml;rzen</trd></trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_s;&c_l;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_k;&c_l;&c_je;&c_v;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="fam.sen0ulo">
  <kap>sen<tld/>ulo<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Nekonata homo.  
    </dif>
  </snc>
  <trd lng="de">Niemand</trd>
</drv>
</art>

