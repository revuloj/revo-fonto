<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fust.xml,v 1.20 2023/11/21 15:28:35 revo Exp $">
<kap><rad>fust</rad>/o</kap>
<drv mrk="fust.0o">
  <kap><tld/>o</kap>
  <snc mrk="fust.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Parto de la kolono inter la bazo kaj la kapitelo.
    </dif>
    <trd lng="be">&c_s;&c_t;&c_v;&c_o;&c_l; &c_k;&c_a;&c_l;&c_jo;&c_n;&c_y;</trd>
    <trdgrp lng="de">
      <trd>Schaft</trd>,
      <trd>S&auml;ulenschaft</trd>
    </trdgrp>
    <trd lng="en">shaft</trd>
    <trd lng="hu">oszlopt&ouml;rzs</trd>
    <trd lng="nl">zuilschacht</trd>
    <trd lng="pl">trzon</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_l;&c_o;&c_n;&c_n;&c_a;</trd>,
      <trd>&c_s;&c_t;&c_je;&c_r;&c_zh;&c_je;&c_n;&c_mol; &c_k;&c_o;&c_l;&c_o;&c_n;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="zh">&#x67F1;&#x8EAB; <pr>zh&#xF9;sh&emacron;n</pr></trd>
</snc>
  <snc mrk="fust.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Parto de iuj objektoj, je kiu oni kaptas, tenas mane:
      <ekz>
        <ind><tld/>o de pafilo</ind>,
        <trd lng="de">Gewehrgriff</trd>
        <trd lng="hu">puskatus</trd>
        <trd lng="ru">&c_c;&c_je;&c_v;&c_mol;&c_jo; <klr>(&c_r;&c_u;&c_zh;&c_je;&c_j;&c_n;&c_o;&c_g;&c_o; &c_l;&c_o;&c_zh;&c_a;)</klr></trd>
      </ekz>
      <ekz>
        <ind><tld/>o de lanco</ind>,
        <trd lng="de">Lanzenschaft</trd>
        <trd lng="hu">l&aacute;ndzsany&eacute;l</trd>
        <trd lng="nl">schacht<klr tip="ind"> (v.e. lans)</klr></trd>
        <trd lng="ru">&c_d;&c_r;&c_je;&c_v;&c_k;&c_o; <klr>(&c_k;&c_o;&c_p;&c_mol;&c_ja;)</klr></trd>
      </ekz>
      <ekz>
        <ind><tld/>o de kandelabro, pokalo</ind>.
        <trdgrp lng="de">
          <trd>Kerzenhalter</trd>,
          <trd>Pokalgriff</trd>
        </trdgrp>
        <trd lng="ru">&c_n;&c_o;&c_zh;&c_k;&c_a;
        <klr>(&c_k;&c_a;&c_n;&c_d;&c_je;&c_l;&c_ja;&c_b;&c_r;&c_a;, &c_r;&c_ju;&c_m;&c_k;&c_i;)</klr></trd>
      </ekz>
    </dif>
        <trdgrp lng="be">
          <trd>&c_s;&c_t;&c_r;&c_y;&c_zh;&c_a;&c_n;&c_mol;</trd>,
          <trd>&c_r;&c_u;&c_ch;&c_k;&c_a;</trd>,
          <trd>&c_c;&c_a;&c_w;&c_jo;</trd>,
          <trd>&c_k;&c_a;&c_s;&c_ib;&c_l;&c_mol;&c_n;&c_a;</trd>
        </trdgrp>
    <trdgrp lng="en">
      <trd>shaft</trd>,
      <trd>stock</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>sz&aacute;r</trd>,
      <trd>ny&eacute;l</trd>,
      <trd>markolat</trd>
    </trdgrp>
    <trd lng="nl">greep</trd>
    <trdgrp lng="pl">
      <trd>trzonek</trd>,
      <trd>uchwyt</trd>,
      <trd>r&eogonek;koje&sacute;&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_r;&c_u;&c_k;&c_o;&c_ja;&c_t;&c_k;&c_a;</trd>,
      <trd>&c_r;&c_u;&c_ch;&c_k;&c_a;</trd>,
      <trd>&c_ch;&c_je;&c_r;&c_je;&c_n;&c_o;&c_k;</trd>
    </trdgrp>
  </snc>
  <refgrp tip="vid">
    <ref cel="ans.0o">anso</ref>,
    <ref cel="baston.0o">bastono</ref>,
    <ref cel="ten.0ilo">tenilo</ref>,
    <ref cel="tig.0o">tigo</ref>,
    <ref cel="trunk.0o">trunko</ref>.
  </refgrp>
  <trd lng="fr">f&ucirc;t<klr tip="ind"> (de colonne, manche)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x67F1;&#x8EAB;  <pr>&#12385;&#12421;&#12358;&#x307F;</pr></trd>,
    <trd>&#x53D6;&#12387;&#x624B;  <pr>&#12392;&#12387;&#12390;</pr></trd>,
    <trd>&#x63E1;&#x308A;  <pr>&#x306B;&#x304E;&#x308A;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>driek st&lacute;pa</trd>,
    <trd>pa&zcaron;ba</trd>,
    <trd>rukov&auml;&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>d&rcaron;&iacute;k sloupu</trd>,
    <trd>no&zcaron;ka &ccaron;&iacute;&scaron;e</trd>,
    <trd>pa&zcaron;ba</trd>,
    <trd>rukov&ecaron;&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&#x43E;&c_v;&c_b;&c_u;&c_r;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_i;&c_zh;&c_je;&#x43D;&#x44C;</trd>,
    <trd>&#x43B;&#x43E;&c_zh;&c_je; (&c_r;&c_u;&c_sh;&#x43D;&c_i;&c_c;&c_ib;)</trd>,
    <trd>&c_r;&c_u;&#x43A;&#x43E;&#x44F;&c_t;&#x43A;&c_a;</trd>,
    <trd>&c_r;&c_u;&c_ch;&#x43A;&c_a; (&c_ib;&#x43D;&c_s;&c_t;&c_r;&c_u;&#x43C;&c_je;&#x43D;&c_t;&c_a;)</trd>,
    <trd>&#x43B;&c_u;&c_ch;&#x43E;&#x43A; (&#x43F;&c_i;&#x43B;&#x43A;&c_i;)</trd>
  </trdgrp>
</drv>
</art>
