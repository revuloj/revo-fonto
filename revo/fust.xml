<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>fust</rad>/o
</kap>
<drv mrk="fust.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Parto de la kolono inter la bazo kaj la kapitelo.
    </dif>
  </snc>
  <snc>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Parto de iuj objektoj, je kiu oni kaptas, tenas mane:
      <ekz>
        <tld/>o de pafilo, kandelabro, pokalo.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="ans.0o">anso</ref>,
    <ref cel="baston.0o">bastono</ref>,
    <ref cel="sxaft.0o">&scirc;afto</ref>,
    <ref cel="ten.0ilo">tenilo</ref>,
    <ref cel="tig.0o">tigo</ref>,
    <ref cel="trunk.0o">trunko</ref>.
  </refgrp>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
