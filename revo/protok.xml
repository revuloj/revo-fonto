<drv mrk="protok.viv0o">
  <kap>viv<tld/>o</kap>
  <snc>
    <dif>
      Konciza listo de gravaj eventoj dum sia vivo, ekz. pri edukado,
      oficoj kaj profesiaj atingoj prezentenda
      de kandidato por ofico a&ubreve; laborloko:
      <ekz>
        la novaj kandidatoj de la akademiaj elektoj devas
        prezenti, krom sia konsento kandidati, anka&ubreve; sian
        viv<tld/>on;
      </ekz>
      <ekz>
        kariertabelo (a&ubreve; viv<tld/>o, vivresumo) estas dokumento verkata
        por prezenti sin al dunganto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=264726">Kariertabelo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la viv<tld/>oj de <klr>[&#x2026;]</klr> ekzilitoj
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="&Monato;/2011/010390.php">Kvarlingva
          muzeo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_z;&c_ju;&c_m;&c_e; <klr>(&c_p;&c_r;&c_a;&c_c;&c_o;&c_w;&c_n;&c_a;&c_je;)</klr></trd>
  <trd lng="cs">&uacute;&rcaron;edn&iacute; &zcaron;ivotopis</trd>
  <trdgrp lng="de">
    <trd>Lebenslauf</trd>,
    <trd>CV <klr>(Lebenslauf)</klr></trd>,
    <trd>Berufserfahrung <klr>(Lebenslauf)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>curriculum vitae</trd>, <trd>resume</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>curriculum vitae</trd>,
    <trd>curr&iacute;culo</trd>,
    <trd>CV</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>curriculum vitae</trd>,
    <trd>CV</trd>
  </trdgrp>
  <trd lng="hu">szakmai <ind>&ouml;n&eacute;letrajz</ind></trd>
  <trd lng="ja">&#x5C65;&#x6B74;&#x66F8;  <pr>&#x308A;&#x308C;&#x304D;&#x3057;&#x3087;</pr></trd>
  <trd lng="nl">curriculum vitae</trd>
  <trdgrp lng="pl">
