<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>tunik</rad>/o
</kap>
<drv mrk="tunik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Penda vesto kovranta la trunkon kaj la femurojn &gcirc;isgenue,
      kun mallongaj manikoj,
      interalie uzata dum Grek-Romia antikvo kiel gea
      <ref tip="super" cel="vest.sub0o">subvesto</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_n;&c_ib;&c_k;&c_a;</trd>
  <trd lng="de">Tunika</trd>
  <trdgrp lng="fr">
    <trd>tunique</trd>
  </trdgrp>
  <trd lng="hu">tunika</trd>
  <trd lng="nl">tunica</trd>
  <trd lng="ru">&c_t;&c_u;&c_n;&c_i;&c_k;&c_a;</trd>
</drv>
</art>
