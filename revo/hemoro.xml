<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hemoroid</rad>/oj
</kap>
<drv mrk="hemoro.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Vejnaj varikoj de la anuso, kiuj sangadas.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_m;&c_a;&c_r;&c_o;&c_j;</trd>
  <trd lng="el">&alfa;&jota;&my;&omikron;&rho;&rho;&omikron;&jota_trema_ton;&delta;&epsilon;&sigma_fina;</trd>
  <trd lng="de">H&auml;morrhoiden</trd>
  <trd lng="nl">aambeien</trd>
  <trd lng="fr">h&eacute;morro&iuml;des</trd>
  <trdgrp lng="hu">
    <trd>arany&eacute;r</trd>,
    <trd>hemorroid&aacute;k</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>guzki krwawnicze</trd>,
    <trd>hemoroidy</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>hemorr&oacute;idas</trd>,
    <trd>hemorr&oacute;ides</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_je;&c_m;&c_o;&c_r;&c_r;&c_o;&c_j;</trd>
</drv>

<drv mrk="hemoro.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj.
    </dif>
  </snc>
  <snc>
    <dif>
      Havanta formon de <tld/>oj.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_m;&c_a;&c_r;&c_a;&c_ib;&c_d;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trd lng="de">h&auml;morrhoidisch</trd>
  <trdgrp lng="fr">
    <trd>h&eacute;morro&iuml;dal</trd>,
    <trd>h&eacute;morro&iuml;daire</trd></trdgrp>
  <trdgrp lng="hu">
    <trd>aranyeres</trd>,
    <trd>hemorroid&aacute;lis</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>hemorroidal</trd>, 
   <trd>hemorroid&aacute;rio<klr tip="ind"> (adj.)</klr></trd> 
  </trdgrp>
  <trd lng="ru">&c_g;&c_je;&c_m;&c_o;&c_r;&c_r;&c_o;&c_i;&c_d;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
