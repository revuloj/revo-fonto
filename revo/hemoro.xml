<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hemoro.xml,v 1.22 2023/11/15 16:45:18 revo Exp $">
<kap>
  <rad>hemoroid</rad>/oj
</kap>
<drv mrk="hemoro.0oj">
  <kap>
        <tld/>oj, <var>
          <kap>hemorojdoj</kap>
        </var>
  </kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Vejnaj varikoj de la anuso, kiuj sangadas:
          <ekz>
            &mdash; La arto akiri monon, a&ubreve; ne ekzistas plu la hemorojdoj! &mdash;
            ekkriis Bazarov kun malestima rideto
            <fnt><bib>PatrojFiloj</bib>, <lok>&Ccirc;ap. 4a.</lok></fnt>.
          </ekz>
    </dif>
    <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_m;&c_a;&c_r;&c_o;&c_j;</trd>
  <trd lng="ca">hemorroide</trd>
  <trd lng="de">H&auml;morrhoiden</trd>
  <trd lng="el">&alfa;&jota;&my;&omikron;&rho;&rho;&omikron;&jota_trema_ton;&delta;&epsilon;&sigma_fina;</trd>
  <trd lng="en">hemmorhoids</trd>
  <trd lng="fa">&a_b;&a_w;&a_A;&a_s;&f_y;&a_r;</trd>
  <trd lng="fr">h&eacute;morro&iuml;des</trd>
  <trdgrp lng="hu">
    <trd>arany&eacute;r</trd>,
    <trd>hemorroid&aacute;k</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>ambeien</trd>,
    <trd>bawasir</trd>,
    <trd>hemoroid</trd>,
    <trd>wasir</trd>
  </trdgrp>
  <trd lng="it">emorroidi</trd>
  <trdgrp lng="ja">
    <trd>&#x75D4;&#x6838;  <pr>&#x3058;&#x304B;&#x304F;</pr></trd>,
    <trd>&#x75D4;&#x75BE;  <pr>&#x3058;&#x3057;&#x3064;</pr></trd>
  </trdgrp>
  <trd lng="nl">aambeien</trd>
  <trdgrp lng="pl">
    <trd>guzki krwawnicze</trd>,
    <trd>hemoroidy</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>hemorr&oacute;idas</trd>,
    <trd>hemorr&oacute;ides</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_je;&c_m;&c_o;&c_r;&c_r;&c_o;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x75D4; <pr>zh&#xEC;</pr></trd>,
    <trd>&#x75D4;&#x75AE; <pr>zh&#xEC;chu&#x101;ng</pr></trd>,
    <trd>&#x75D4;&#x75AE; <pr>zh&#xEC;chuang</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hemoro.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj.
    </dif>
  </snc>
  <snc>
    <dif>
      Havanta formon de <tld/>oj.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_m;&c_a;&c_r;&c_a;&c_ib;&c_d;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trd lng="ca">hemorro&iuml;dal</trd>
  <trd lng="de">h&auml;morrhoidisch</trd>
  <trd lng="en">hemmorhoidal</trd>
  <trd lng="fa">&a_b;&a_w;&a_A;&a_s;&f_y;&a_r;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>h&eacute;morro&iuml;dal</trd>,
    <trd>h&eacute;morro&iuml;daire</trd></trdgrp>
  <trdgrp lng="hu">
    <trd>aranyeres</trd>,
    <trd>hemorroid&aacute;lis</trd>
  </trdgrp>
  <trd lng="it">emorroidale</trd>
  <trd lng="ja">&#x75D4;&#x306E;  <pr>&#x3058;&#x306E;</pr></trd>
  <trdgrp lng="pt">
   <trd>hemorroidal</trd>, 
   <trd>hemorroid&aacute;rio<klr tip="ind"> (adj.)</klr></trd> 
  </trdgrp>
  <trd lng="ru">&c_g;&c_je;&c_m;&c_o;&c_r;&c_r;&c_o;&c_i;&c_d;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trd lng="uk">&#x433;&#x435;&#x43C;&#x43E;&#x440;&#x43E;&#x457;&#x434;&#x430;&#x43B;&#x44C;&#x43D;&#x438;&#x439;</trd>
</drv>
</art>
