<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hemoroid</rad>/oj
</kap>
<drv mrk="hemoro.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Vejnaj varikoj de l' anuso, kiuj sangadas. 
    </dif>
  </snc>
  <trd lng="ru">&c_g;&c_je;&c_m;&c_o;&c_r;&c_r;&c_o;&c_j;</trd>
</drv>
<drv mrk="hemoro.0a">
  <kap><tld/>a</kap>
  <snc num="1">
    <dif>
      Rilatanta al <tld/>oj. 
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Havanta formon de <tld/>oj.  
    </dif>
  </snc>
  <trd lng="ru">&c_g;&c_je;&c_m;&c_o;&c_r;&c_r;&c_o;&c_i;&c_d;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
