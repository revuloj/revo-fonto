<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>balen</rad>/o
</kap>
<drv mrk="balen.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="balen.0o.KOMUNE">
     <uzo tip="stl">KOMUNE</uzo>
     <dif>
       Iu el la ordo <ref tip="malprt" cel="cetac.0oj">cetacoj</ref>
     </dif>
     <trd lng="de">Wal</trd>
   </snc>
   <snc mrk="balen.0o.genro">
     <uzo tip="fak">ZOO</uzo>
     <dif>
       Genro el la familio <ref tip="super" cel="balen.0edoj"><tld/>edoj</ref>
       (<trd lng="la">Balaena</trd>), kies specioj preska&ubreve; elmortis.
     </dif>
     <trd lng="de">Glattwal</trd>
   </snc>
   <trd lng="fr">baleine<klr tip="ind"> (c&eacute;tac&eacute;)</klr></trd>
   <trd lng="ru">&c_k;&c_i;&c_t;</trd>
</drv>
<drv mrk="balen.0ajxo">
   <kap><tld/>a&jcirc;o</kap>
   <snc>
     <ref tip="dif" cel="bart.0o">barto.</ref>
   </snc>
   <trd lng="ru">&c_k;&c_i;&c_t;&c_o;&c_v;&c_y;&c_j; &c_u;&c_s;</trd>
</drv>

</art>
