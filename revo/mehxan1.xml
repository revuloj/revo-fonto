<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>me&hcirc;anism</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="mehxan1.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="mehxan1.0o.pecoj">
    <dif>
      Tuto de la pecoj de ma&scirc;ino a&ubreve; aparato
    </dif>
    <refgrp tip="sin">
      <ref cel="mehxan.0ajxo">me&hcirc;anika&jcirc;o</ref>;
    </refgrp>
    <refgrp tip="vid">
      <ref cel="rad.0aro">radaro</ref>,
      <ref cel="mov">movilaro</ref>.
    </refgrp>
    <trd lng="ca">mecanisme</trd>
    <trd lng="de">Mechanismus</trd>
    <trd lng="es">mecanismo</trd>
    <trd lng="sv">mekanism</trd>
  </snc>
  <snc mrk="mehxan1.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Rimedaro, per kiu plenumi&gcirc;as iu ago a&ubreve; funkcio;
      strukturo de kunaran&gcirc;itaj elementoj kaj ilia maniero kune funkcii:
      <ekz>
        <tld/>o de la digestado<fnt>B</fnt>;
      </ekz>
      <ekz>
        la <tld/>o de la lingvo<fnt>Z</fnt>,
      </ekz>
      <ekz>
        de la penso.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="mehxan.0o.FIG">me&hcirc;aniko</ref>.
    </refgrp>
    <trd lng="ca">mecanisme</trd>
    <trd lng="es">mecanismo</trd>
    <trd lng="de">Mechanismus</trd>
    <trd lng="sv">mekanism</trd>
  </snc>
  <snc mrk="mehxan1.0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Filozofia teorio, kiu strebas komprenigi tuton per studado de
      &gcirc;iaj partoj.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_h;&c_a;&c_n;&c_ib;&c_z;&c_m;</trd>
  <trd lng="en">mechanism</trd>
  <trd lng="fr">m&eacute;canisme</trd>
  <trd lng="nl">mechanisme</trd>
  <trd lng="pt">mecanismo</trd>
  <trd lng="ru">&c_m;&c_je;&c_h;&c_a;&c_n;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
