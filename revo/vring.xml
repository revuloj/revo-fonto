<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>vring</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="vring.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="vring.0i.KOMUNE">
    <ref tip="dif" cel="tord.kun0i.FIZ">kuntordi</ref>
    <ekz>
      &scirc;i &jcirc;us <tld/>is la trempitan tukon
      <fnt>
      <aut>J. Rejt&odblac;, trad. L. Bal&aacute;zs</aut>,
      <vrk><url ref="http://www.freeweb.hu/verkoj/22.htm">
      Bill Teksaso, la temerarulo</url></vrk>,
      <lok>2002-10-16</lok>
      </fnt>;
    </ekz>
    <ekz>
      <uzo tip="stl">FIG</uzo>
      doloreto <tld/>is al li la kapon
      <fnt>
      <aut>Guo Moruo</aut>,
      <vrk><url ref="http://esperanto.cri.cn/1/2003/12/26/1@2081.htm">
      En &ccirc;ambreto (1)</url></vrk>,
      <lok>2003-12</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="fr">
    <trd>essorer</trd>,
    <trd>tordre<klr> (un linge etc.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="vring.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="vring.el0i.KOMUNE">
    <dif>
      Torde <ref tip="vid" cel="prem.el0i">elpremi</ref>;
      <ref tip="vid" cel="dev.el0igi.KOMUNE">eldevigi</ref>:
      <ekz>
        el<tld/>i la lastan guton
        <fnt>
          <aut>K. Miner</aut>,
          <vrk><url
          ref="http://www.sunflower.com/~miner/TAGLIBRO_package/Aprilo_2008.html">
          Taglibro</url></vrk>,
          <lok>2008-04-17</lok>
        </fnt>;
      </ekz>
      <ekz>
        el<tld/>u la sukon
        <fnt>
          <aut>Tajlvent', trad. F. Gaulthier</aut>,
          <vrk><url
          ref="http://www.oocities.org/paris/8059/tajlvent.html">
          receptoj</url></vrk>,
          <lok>[vidita en 2014]</lok>
        </fnt>.
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        oni &ccirc;ion elvringu <klr>...</klr> el la komercistoj
        <fnt>
          <aut>B. Traven, trad. D. kaj H.-G. Kaiser</aut>,
          <vrk><url
          ref="http://cindymckee.com/librejo/btraven_registaro.pdf">
          Registaro</url></vrk>,
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>exprimer<klr> (extraire par pression)</klr></trd>,
    <trd>extorquer</trd>,
    <trd>pressurer</trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
-->
</vortaro>