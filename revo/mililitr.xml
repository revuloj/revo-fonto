<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mililitr.xml,v 1.6 2024/01/26 17:40:06 revo Exp $">
<kap>
  <rad>mililitr</rad>/o <fnt><bib>GDFE</bib></fnt>
</kap>
<drv mrk="mililitr.0o">
  <kap><tld/>o</kap>
  <mlg>ml</mlg>
  <snc mrk="mililitr.0o.MEZ">
    <dif>
      <ref tip="lst" cel="unu.mezur0o" lst="voko:kvantoj">Mezurunuo</ref> de volumeno,
      <ref tip="malprt" cel="litr.0o.MEZ">litro</ref>milono:
      <ekz>
        1 supkulero = 15 <tld/>oj
        <fnt>
          <aut>Misick</aut>,
          <vrk><url ref="http://www.misick.org/ni-kuiru/kuko-de-jahurto.eo.html">
          Kuko de Jahurto</url></vrk>,
          <lok>La Junuloj, 2001-03, numero 54a, p. 13a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_l;&c_ib;&c_l;&c_ib;&c_t;&c_a;&c_r;</trd>
  <trd lng="ca">mil&middot;lilitre</trd>
  <trd lng="de">Milliliter</trd>
  <trd lng="fr">millilitre</trd>
  <trd lng="it">millilitro</trd>
  <trdgrp lng="zh">
    <trd>&#x6BEB;&#x5347; <pr>h&#xE1;osh&#x113;ng</pr></trd>,
    <trd>&#x516C;&#x64AE; <pr>g&#x14D;ngcu&#x14D;</pr></trd>
  </trdgrp>
</drv>
</art>

