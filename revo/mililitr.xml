<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mililitr.xml,v 1.3 2012/08/20 00:10:16 revo Exp $">
<kap>
  <rad>mililitr</rad>/o <fnt><bib>GDFE</bib></fnt>
</kap>
<drv mrk="mililitr.0o">
  <kap><tld/>o</kap>
  <mlg>ml</mlg>
  <snc mrk="mililitr.0o.MEZ">
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de kapacito, 
      <ref tip="malprt" cel="litr.0o.MEZ">litro</ref>milono:
      <ekz>
        1 supkulero = 15 <tld/>oj
        <fnt>
          <aut>Misick</aut>,
          <vrk><url
          ref="http://www.misick.org/ni-kuiru/kuko-de-jahurto.eo.html">
          Kuko de Jahurto</url></vrk>,
          <lok>La Junuloj, 2001-03, numero 54a, p. 13a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">mil&middot;lilitre</trd>
  <trd lng="de">Milliliter</trd>
  <trd lng="fr">millilitre</trd>
  <trd lng="it">millilitro</trd>
</drv>
</art>

