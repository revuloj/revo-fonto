<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>kosm</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kosm.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <ref tip="dif" cel="univer.0o">universo</ref>
    <ekz>
      <ctl>la <tld/>o kaj ni: galaksioj, planedoj kaj vivo en la universo</ctl>
      <fnt><bib>Kosmo</bib></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_o;&c_s;&c_m;&c_a;&c_s;</trd>,
    <trd>&c_s;&c_u;&c_s;&c_mol;&c_v;&c_je;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Weltall</trd>,
    <trd>All</trd>,
    <trd>Weltraum</trd>,
    <trd>Kosmos</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>(outer) <ind>space</ind></trd>,
    <trd>universe</trd>,
    <trd>cosmos</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cosmos</trd>,
    <trd>univers</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vil&aacute;gegyetem</trd>,
    <trd>kozmosz</trd>,
    <trd>vil&aacute;g&udblac;r</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>heelal</trd>,
    <trd>kosmos</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_m;&c_o;&c_s;</trd>
</drv>

<drv mrk="kosm.0a">
  <kap><tld/>a</kap>
  <snc>
    <ref tip="dif" cel="univer.0a">universa</ref>
    <ekz>
      <ctl>kosma polico</ctl>
      <fnt><vrk>titolo de novelaro eldonita en Santander:
      Grupo Nifo, 2000</vrk></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_s;&c_mol;&c_m;&c_ib;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_u;&c_s;&c_mol;&c_v;&c_je;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kozmikus</trd>,
    <trd>&udblac;rbeli</trd>
  </trdgrp>
  <trd lng="nl">kosmisch</trd>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
