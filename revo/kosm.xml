<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>kosm</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kosm.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <ref tip="dif" cel="univer.0o">universo</ref>
    <ekz>
      <ctl>la <tld/>o kaj ni: galaksioj, planedoj kaj vivo en la universo</ctl>
      <fnt><aut>David Galad&iacute;-Enr&iacute;quez kaj Amri Wandel</aut>: <vrk>titolo de
      verko eldonita en Antverpeno: Fundaci&oacute;n Esperanto kaj Flandra Esperanto-Ligo, 2001</vrk></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="de">
    <trd>Weltall</trd>,
    <trd>All</trd>,
    <trd>Weltraum</trd>,
    <trd>Kosmos</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cosmos</trd>,
    <trd>univers</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>(outer) space</trd>,
    <trd>universe</trd>,
    <trd>cosmos</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_m;&c_o;&c_s;</trd>
</drv>

<drv mrk="kosm.0a">
  <kap><tld/>a</kap>
  <snc>
    <ref tip="dif" cel="univer.0a">universa</ref>
    <ekz>
      <ctl>kosma polico</ctl>
      <fnt><vrk>titolo de novelaro eldonita en Santander:
      Grupo Nifo, 2000</vrk></fnt>.
    </ekz>    
  </snc>
</drv>
</art>
