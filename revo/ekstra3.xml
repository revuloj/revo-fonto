<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ekstrapol</rad>/i
</kap>
<drv mrk="ekstra3.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>SPV</bib></fnt>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ekstra3.0i.MAT">
    <ref tip="dif" cel="ekster2.0i">eksterpoli</ref>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_s;&c_t;&c_r;&c_a;&c_p;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">extrapolieren</trd>
  <trd lng="en">extrapolate</trd>
  <trd lng="es">extrapolar</trd>
  <trd lng="fa">&fa_be;&fa_re;&fa_vav;&fa_nun;&fa_zwnj;&fa_ye;&fa_alef;&fa_be;&fa_ye;
  &fa_kaf;&fa_re;&fa_dal;&fa_nun;</trd>
  <trd lng="fr">extrapoler</trd>
  <trd lng="hu">extrapol&aacute;l</trd>
  <trd lng="pl">ekstrapolowa&cacute;</trd>
  <trd lng="ru">&c_e;&c_k;&c_s;&c_t;&c_r;&c_a;&c_p;&c_o;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
<adm>
  Mi demandas al mi, &ccirc;u vere ne troveblas iuj "eruditaj" sencoj,
  paralelaj al tiuj en la franca: ~i la konsekvencojn de ies agoj (racie
  konjekti, kio okazos)...
  <aut>MB</aut>
</adm>
</art>
