<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pagure.xml,v 1.14 2022/03/25 10:18:30 revo Exp $">
<kap><rad>pagured</rad>/oj</kap>
<drv mrk="pagure.0oj">
  <kap><tld/>oj</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="krust.dekpiedaj0uloj">dekpiedaj
      krustuloj</ref>
      (<trd lng="la">Paguridae</trd>), vivantaj en
      malplenaj konkoj:
      <ekz>
        &ccirc;e la <tld/>oj la dekstra pin&ccirc;ilo estas pli granda,
        la plej multaj el la pli ol 550 specioj restas pli malgrandaj ol iliaj
        &leftquot;maldekstramanaj&rightquot; parencoj (<nom>Diogenidae</nom>).
      </ekz>
    </dif>
    <ref tip="prt" cel="pagur.0o">paguro</ref>
  </snc>
  <trdgrp lng="en">
    <trd>Paguridae</trd>,
    <trd>hermit crabs</trd>
  </trdgrp>
  <trd lng="be">&c_r;&c_a;&c_k;&c_ib;-&c_p;&c_u;&c_s;&c_t;&c_e;&c_l;&c_mol;&c_n;&c_ib;&c_k;&c_ib; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">pag&uacute;rids</trd>
  <trd lng="de">Rechtsh&auml;ndige <ind>Einsiedlerkrebse</ind></trd>
  <trd lng="fr">Pagures</trd>
  <trd lng="hu">remeter&aacute;kok <klr tip="amb">(csal&aacute;d)</klr></trd>
  <trd lng="nl">heremietkreeften</trd>
  <trd lng="pl">pustelnikowate</trd>
  <trdgrp lng="pt">
   <trd>pagur&iacute;deos</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_k;&c_i;-&c_o;&c_t;&c_sh;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_k;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&#x440;&#x430;&#x43A;&#x438;-&#x441;&#x430;&#x43C;&#x456;&#x442;&#x43D;&#x438;&#x43A;&#x438;</trd>
</drv>
</art>
