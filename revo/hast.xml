<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hast</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="hast.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="hast.0i.KOMUNE">
    <dif>
      Rapidigi siajn movojn, por pli balda&ubreve; plenumi ion;
      agi kun nekutima, nenormala rapideco:
      <ekz>
        je la siblo de la kugloj, li <tld/>is al la tran&ccirc;eo;
      </ekz>
      <ekz>
        ne <tld/>u fari vian taskon, vi disponas la tutan necesan
        tempon.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_p;&c_ja;&c_sh;&c_a;&c_c;&c_c;&c_a;</trd>
  <trd lng="bg">&c_p;&c_o;&c_b;&c_malmol;&c_r;&c_z;&c_a;</trd>
  <trd lng="de">hasten</trd>
  <trdgrp lng="en">
    <trd>rush</trd>,
    <trd>haste</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>apurarse</trd>,
    <trd>darse prisa</trd>,
    <trd>apresurarse</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_sh;&a_t;&a_A;&a_b; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_ayn;&a_j;&a_l;&a_h; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>h&acirc;ter</ind></trd>,
    <trd>se <ind>d&eacute;p&ecirc;cher</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>siet</trd>,
    <trd>igyekszik<klr tip="ind"> (siet)</klr></trd>,
    <trd>kapkod</trd>
  </trdgrp>
  <trd lng="it">affrettarsi</trd>
  <trd lng="nl">zich <ind>haasten</ind></trd>
  <trd lng="pl">spieszy&cacute; si&eogonek;</trd>
  <trd lng="pt">apressar-se</trd>
  <trd lng="ro">a se gr&abreve;bi</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_je;&c_sh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_t;&c_o;&c_r;&c_o;&c_p;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hnať sa</trd>,
    <trd>náhliť sa</trd>
  </trdgrp>
</drv>

<drv mrk="hast.0o">
  <kap><tld/>o</kap>
  <snc mrk="hast.0o.KOMUNE">
    <dif>
      Deziro, ke io estu tuj farata; senpacienca penado atingi celon:
      <ekz>
        letero skribita kun <tld/>o;
      </ekz>
      <ekz>
        &scirc;i sentis <tld/>on edzini&gcirc;i kaj liberi&gcirc;i el la
        familia atmosfero.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_p;&c_je;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_mol;&c_p;&c_je;&c_sh;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_p;&c_r;&c_i;&c_p;&c_r;&c_ja;&c_n;&c_o;&c_s;&c_t;</trd>,
    <trd>&c_b;&c_malmol;&c_r;&c_z;&c_a;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="de">Hast</trd>
  <trd lng="en">haste</trd>
  <trdgrp lng="es">
    <trd>apuramiento</trd>,
    <trd>prisa</trd>,
    <trd>apresuramiento</trd>,
    <trd>premura</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_sh;&a_t;&a_A;&a_b;</trd>,
    <trd>&a_ayn;&a_j;&a_l;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">h&acirc;te</trd>
  <trdgrp lng="hu">
    <trd>siets&eacute;g</trd>,
    <trd>igyekezet</trd>
  </trdgrp>
  <trd lng="it">fretta</trd>
  <trd lng="nl">haast</trd>
  <trd lng="pl">po&sacute;piech</trd>
  <trd lng="pt">pressa</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_je;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_o;&c_r;&c_o;&c_p;&c_l;&c_i;&c_v;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chvat</trd>,
    <trd>náhlenie</trd>
  </trdgrp>
</drv>

<drv mrk="hast.0a">
  <kap><tld/>a</kap>
  <snc mrk="hast.0a.KOMUNE">
    <dif>
      Tre a&ubreve; tro rapida; farita sen matura pripenso
      a&ubreve; preparo:
      <ekz>
        <tld/>a man&gcirc;o, adia&ubreve;o, konkludo;
      </ekz>
      <ekz>
        ju pli <ind><tld/>e</ind>, des malpli rapide.
        <trdgrp lng="fr">
          <trd>en <ind>h&acirc;te</ind></trd>,
          <trd>&agrave; la <ind>h&acirc;te</ind></trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <adm>
    Klarigo bonvena por la ekzemplo: ju pli ~e...
    <aut>MB</aut>
  </adm>
  <trd lng="be">&c_p;&c_a;&c_s;&c_mol;&c_p;&c_je;&c_sh;&c_l;&c_ib;&c_v;&c_y;</trd>
  <trd lng="bg">&c_p;&c_r;&c_i;&c_b;&c_malmol;&c_r;&c_z;&c_a;&c_n;</trd>
  <trd lng="de">hastig</trd>
  <trd lng="en">hasty</trd>
  <trdgrp lng="fa">
    <trd>&a_ayn;&a_j;&a_w;&a_l;&a_A;&a_n;&a_h;</trd>,
    <trd>&a_sh;&a_t;&a_A;&a_b;&zwnj;&a_z;&a_d;&a_h;</trd>,
    <trd>&a_h;&a_w;&a_l;&zwnj;&a_h;&a_w;&a_l;&f_k;&f_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>h&acirc;tif</trd>,
    <trd>pr&eacute;cipit&eacute;<klr tip="ind"> (h&acirc;tif)</klr></trd>,
    <trd>b&acirc;cl&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>siet&odblac;s</trd>,
    <trd>kapkod&oacute;</trd>
  </trdgrp>
  <trd lng="it">affrettato</trd>
  <trdgrp lng="nl">
    <trd>haastig</trd>,
    <trd>gejaagd</trd>,
    <trd>gehaasd</trd>
  </trdgrp>
  <trd lng="pl">spieszny</trd>
  <trdgrp lng="es">
    <trd>apurado, -a</trd>,
    <trd>apresurado</trd>,
    <trd>hecho, -a a la carrera</trd>,
    <trd>apremiado, -a</trd>
  </trdgrp>
  <trd lng="pt">apressado</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_s;&c_p;&c_je;&c_sh;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_o;&c_r;&c_o;&c_p;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rýchlo urobený</trd>,
    <trd>unáhlený</trd>
  </trdgrp>
</drv>

<drv mrk="hast.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="hast.0ema.KOMUNE">
    <dif>
      Rapidema; neniam volanta perdi iom ajn da tempo.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_mol;&c_p;&c_je;&c_sh;&c_l;&c_ib;&c_v;&c_y; <klr>(&c_p;&c_r;&c_a; &c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;&c_a;)</klr></trd>
  <trd lng="de">eilig</trd>
  <trdgrp lng="es">
    <trd>apresurado, -a</trd>,
    <trd>ajolotado, -a</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_n;&a_A;&a_sh;&f_k;&f_y;&a_b;&a_A;</trd>,
    <trd>&a_b;&f_y;&zwnj;&a_S;&a_b;&a_r;</trd>,
    <trd>&a_b;&f_y;&zwnj;&a_t;&a_A;&a_b;</trd>,
    <trd>&a_b;&f_y;&zwnj;&a_q;&a_r;&a_A;&a_r;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>impatient</trd>,
    <trd>press&eacute;<klr tip="ind"> (impatient)</klr></trd>
  </trdgrp>
  <trd lng="hu">&ouml;r&ouml;kk&eacute; siet&odblac;</trd>
  <trd lng="it">frettoloso</trd>
  <trd lng="nl">jachtig</trd>
  <trd lng="pl">spiesz&aogonek;cy si&eogonek;</trd>
  <trd lng="pt">apressado</trd>
  <trd lng="ru">&c_t;&c_o;&c_r;&c_o;&c_p;&c_l;&c_i;&c_v;&c_y;&c_j; <klr>(&c_o; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;&c_je;)</klr></trd>
  <trdgrp lng="sk">
    <trd>chvátajúci</trd>,
    <trd>ponáhľajúci sa</trd>
  </trdgrp>
</drv>

<drv mrk="hast.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="hast.mal0a.KOMUNE">
    <dif>
      Ne rapidema, okazanta la&ubreve; trankvila takto:
      <ekz>
        ni mar&scirc;is mal<tld/>e la&ubreve; la bordo
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">nonchalant</trd>
</drv>
</art>
