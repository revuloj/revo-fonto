<drv mrk="hast.0a">
  <kap><tld/>a</kap>
  <snc mrk="hast.0a.KOMUNE">
    <dif>
      Tre a&ubreve; tro <ref tip="super" cel="rapid.0a">rapida</ref>; farita sen matura pripenso
      a&ubreve; preparo:
      <ekz>
        <tld/>a man&gcirc;o, adia&ubreve;o, konkludo;
      </ekz>
      <ekz>
        &scirc;i povas ripozi kaj son&gcirc;i pri pasintaj tempoj, kiam oni ne tiom multe pelis &scirc;in
        kiel en niaj <tld/>aj tagoj
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 19</lok>
        </fnt>;
      </ekz>
      <ekz>
        li eksperimentis <klr>[&#x2026;]</klr> ekspresionisman esprimmanieron, kun nervaj
        frazkonstruoj, <tld/>aj, malegalaj paragrafoj, kiuj sekvas la ritmon de la
        pensodisvolvi&gcirc;o
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ju pli <ind><tld/>e</ind>, des malpli rapide.
        <trdgrp lng="fr">
          <trd>en <ind>h&acirc;te</ind></trd>,
          <trd>&agrave; la <ind>h&acirc;te</ind></trd>
        </trdgrp>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hektik.0a.FIG">hektika<sncref/></ref>,
      <ref cel="urgx.0a">ur&gcirc;a</ref>
    </refgrp>
  </snc>
  <adm>
    Klarigo bonvena por la ekzemplo: ju pli ~e...
    <aut>MB</aut>
  </adm>
  <trd lng="be">&c_p;&c_a;&c_s;&c_mol;&c_p;&c_je;&c_sh;&c_l;&c_ib;&c_v;&c_y;</trd>
  <trd lng="bg">&c_p;&c_r;&c_i;&c_b;&c_malmol;&c_r;&c_z;&c_a;&c_n;</trd>
  <trd lng="de">hastig</trd>
  <trd lng="en">hasty</trd>
  <trdgrp lng="fa">
    <trd>&a_ayn;&a_j;&a_w;&a_l;&a_A;&a_n;&a_h;</trd>,
    <trd>&a_sh;&a_t;&a_A;&a_b;&#x200C;&a_z;&a_d;&a_h;</trd>,
    <trd>&a_h;&a_w;&a_l;&#x200C;&a_h;&a_w;&a_l;&f_k;&f_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>h&acirc;tif</trd>,
    <trd>pr&eacute;cipit&eacute;<klr tip="ind"> (h&acirc;tif)</klr></trd>,
    <trd>b&acirc;cl&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>siet&odblac;s</trd>,
    <trd>kapkod&oacute;</trd>
  </trdgrp>
  <trd lng="it">affrettato</trd>
  <trdgrp lng="ja">
    <trd>&#x5927;&#x6025;&#x304E;&#x306E;  <pr>&#x304A;&#x304A;&#x3044;&#x305D;&#x304E;&#x306E;</pr></trd>,
    <trd>&#x62D9;&#x901F;&#x306E;  <pr>&#x305B;&#x3063;&#x305D;&#x304F;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
