    <trd>&a_sh;&a_t;&a_A;&a_b;&#8204;&a_z;&a_d;&a_h;</trd>,
    <trd>&a_h;&a_w;&a_l;&#8204;&a_h;&a_w;&a_l;&f_k;&f_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
