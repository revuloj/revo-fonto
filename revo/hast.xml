    <trd>cepat-cepat</trd>,
    <trd><baz>cepat-cepat</baz>bercepat-cepat</trd>,
    <trd><baz>gegas</baz>bergegas</trd>,
    <trd><baz>gegas</baz>tergegas</trd>,
