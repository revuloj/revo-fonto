<drv mrk="prezen1.mis0i">
  <kap>mis<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Mal&gcirc;uste, false <tld/>i:
      <ekz>
        a&ubreve; pro nescio a&ubreve; pro pasio li mis<tld/>is la decidojn de
        la Akademio<fnt><bib>PIV1</bib></fnt>;
      </ekz>
      <ekz>
        vortaroj mis<tld/>as tiujn homonimojn samradikaj vortoj;
      </ekz>
      <ekz>
        plejparte forestis provoj moki a&ubreve; mis<tld/>i la vidpunktojn de
        la indi&gcirc;enoj
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="https://www.monato.be/2004/008193.html">Pri
          indi&gcirc;enaj meksikanoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        tio permesas al la legantoj dise tra la mondo pli bone kompreni
        la belgan 
        problemaron, kiu estas ofte mis<tld/>ita de alilandaj
        informfontoj
        <fnt>
          <bib>Monato</bib>
          <aut>Marc Demonty</aut>
          <vrk><url ref="&Monato;/2007/009368.php">&Ccirc;u Flandrio kaj
          Valonio divorcu? (2)</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi esperas, ke tiajn mis<tld/>ojn oni ne plu aperigos
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="https://www.monato.be/2003/008039.html">La
          gepatra de Zamenhof (2)</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>deformar</trd>,
    <trd>tergiversar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nespr&aacute;vn&ecaron; p&rcaron;elo&zcaron;it</trd>,
    <trd>p&rcaron;ekroutit</trd>,
    <trd>zkomolit smysl</trd>,
    <trd>zkreslit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verzerren</trd>,
    <trd>falsch darstellen</trd>
  </trdgrp>
  <trd lng="en">misrepresent</trd>
  <trdgrp lng="hu">
    <trd>f&eacute;lre&eacute;rtelmez</trd>,
    <trd>t&eacute;vesen <ind>ismertet</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_d;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol; &c_v; &c_l;&c_o;&c_zh;&c_n;&c_o;&c_m; &c_s;&c_v;&c_je;&c_t;&c_je;</trd>,
    <trd>&c_i;&c_s;&c_k;&c_a;&c_z;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>skomoli&tcaron; zmysel</trd>,
    <trd>skresli&tcaron;</trd>
  </trdgrp>
</drv>

