<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: racion.xml,v 1.31 2025/03/10 06:30:56 revo Exp $">
<kap>
  <rad>racional</rad>/a <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="racion.0a">
  <kap><tld/>a</kap>
  <snc mrk="racion.0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="racion.0a.nombro">
      <fnt><bib>MatTerm</bib><lok>p. 7</lok></fnt>
      <dif>
        <klr>(p.p. <ref cel="nombr.0o.MAT">nombro</ref>)</klr>
        Estanta kvociento de du <ref cel="entjer.0o">entjeroj</ref>.
      </dif>
      <ref tip="sin" cel="racio5.0a">raciona</ref>
    </subsnc>
    <subsnc mrk="racion.0a.diversaj">
      <refgrp tip="vid">
        <ref cel="frakci.racionala0o"><tld/>a frakcio</ref>,
        <ref cel="funkci.racionala0o"><tld/>a funkcio</ref>
      </refgrp>
      <ekz>
        <tld/>a funkcio estas funkcio esprimebla kiel frakcio, kies numeratoro kaj
        denominatoro estas polinomoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=337949"
              >Racionala funkcio</url></lok>
        </fnt>.
      </ekz>
    </subsnc>
    <trd lng="pl">wymierny</trd>
  </snc>
  <snc>
    <dif>
      <ref tip="sin" cel="raci.0a">Racia</ref>.
    </dif>
    <trd lng="pl">racjonalny</trd>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_c;&c_y;&c_ja;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trd lng="ca">racional</trd>
  <trdgrp lng="cs">
    <trd>racion&aacute;ln&iacute;</trd>,
    <trd>soum&ecaron;&rcaron;iteln&yacute;</trd>
  </trdgrp>
  <trd lng="de">rational</trd>
  <trd lng="en">rational</trd>
  <trd lng="es">racional</trd>
  <trd lng="fr">rationnel</trd>
  <trd lng="hu">racion&aacute;lis</trd>
  <trd lng="ja">&#26377;&#29702;&#12398;  <pr>&#12422;&#12358;&#12426;&#12398;</pr></trd>
  <trd lng="nl">rationeel</trd>
  <trdgrp lng="pt">
   <trd>racional</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_c;&c_i;&c_o;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">racion&aacute;lny</trd>
  <trd lng="sv">rationell</trd>
  <trd lng="uk">&c_r;&c_a;&c_c;&c_ib;&c_o;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j;</trd>
</drv>

<drv mrk="racion.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 8</lok></fnt>
  <snc>
    <dif>
      <ref cel="racion.0a.nombro"><tld lit="R"/>a</ref>
      <ref tip="super" cel="nombr.0o.MAT">nombro</ref>:
      <ekz>
        demonstri ke ekzistas ne<tld/>oj a kaj b, tiaj ke a<sup>b</sup> estas <tld/>o
        <fnt>
          <aut>Christian Rivi&egrave;re</aut>
          <vrk><url ref="https://www.eventoj.hu/steb/matematiko/matematika-rezonado-eo-riviere.pdf"
              >Matematika rezonado</url></vrk>
          <lok>Skoloj</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="racio5.0o">raciono</ref>
    <ref tip="super" cel="reel.0o">reelo</ref>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_c;&c_y;&c_ja;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;
<ind>&c_l;&c_ib;&c_k;</ind></trd>
  <trd lng="ca">nombre <ind>racional</ind></trd>
  <trd lng="cs">racion&aacute;ln&iacute; &ccaron;&iacute;slo</trd>
  <trd lng="de">rationale <ind>Zahl</ind></trd>
  <trd lng="en">rational <ind>number</ind></trd>
  <trd lng="es">n&uacute;mero racional</trd>
  <trd lng="fr">nombre rationnel</trd>
  <trd lng="hu">racion&aacute;lis <ind>sz&aacute;m</ind></trd>
  <trd lng="ja">&#26377;&#29702;&#25968;  <pr>&#12422;&#12358;&#12426;&#12377;&#12358;</pr></trd>
  <trd lng="nl">rationeel <ind>getal</ind></trd>
  <trd lng="pl">liczba wymierna</trd>
  <trd lng="ru">&c_r;&c_a;&c_c;&c_i;&c_o;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_je;
<ind>&c_ch;&c_i;&c_s;&c_l;&c_o;</ind></trd>
  <trd lng="sk">racion&aacute;lne &ccaron;&iacute;slo</trd>
  <trd lng="sv">rationellt <ind>tal</ind></trd>
  <trdgrp lng="zh">
    <trd>&#26377;&#29702;&#25968; <pr>y&#466;ul&#464;sh&ugrave;</pr></trd>,
    <trd>&#26377;&#29702;&#25976; <pr>y&#466;ul&#464;sh&ugrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="racion.ne0o">
  <kap>ne<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 8</lok></fnt>
  <snc>
    <dif>
      <ref cel="reel.0a">Reela</ref>
      <ref tip="super" cel="nombr.0o.MAT">nombro</ref>, kiu ne estas
      <tld/>a:
      <ekz>
        oni pruvas per <ref cel="redukt.0oalabsurdo">redukto al
        absurdo</ref>, ke la kvadrata <ref cel="radik.0o.per">radiko</ref> de
        <frm>2</frm> estas ne<tld/>o.
      </ekz>
    </dif>
    <ref tip="super" cel="reel.0o">reelo</ref>
    <ref tip="vid" cel="trancx.0o.dedekinda">tran&ccirc;o</ref>
  </snc>
  <trd lng="be">&c_ib;&c_r;&c_a;&c_c;&c_y;&c_ja;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y; <ind>&c_l;&c_ib;&c_k;</ind></trd>
  <trd lng="ca">nombre <ind>irracional</ind></trd>
  <trd lng="cs">iracion&aacute;ln&iacute; &ccaron;&iacute;slo</trd>
  <trd lng="de">irrationale <ind>Zahl</ind></trd>
  <trd lng="en">irrational <ind>number</ind></trd>
  <trd lng="es">n&uacute;mero irracional</trd>
  <trd lng="fr">nombre irrationnel</trd>
  <trd lng="hu">irracion&aacute;lis <ind>sz&aacute;m</ind></trd>
  <trd lng="ja">&#28961;&#29702;&#25968;  <pr>&#12416;&#12426;&#12377;&#12358;</pr></trd>
  <trd lng="pl">liczba niewymierna</trd>
  <trd lng="ru">&c_i;&c_r;&c_r;&c_a;&c_c;&c_i;&c_o;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_je; <ind>&c_ch;&c_i;&c_s;&c_l;&c_o;</ind></trd>
  <trd lng="sk">iracion&aacute;lne &ccaron;&iacute;slo</trd>
  <trd lng="sv">irrationellt <ind>tal</ind></trd>
  <trdgrp lng="zh">
    <trd>&#26080;&#29702;&#25968; <pr>w&uacute;l&#464;sh&ugrave;</pr></trd>,
    <trd>&#28961;&#29702;&#25976; <pr>w&uacute;l&#464;sh&ugrave;</pr></trd>
  </trdgrp>
</drv>

</art>
