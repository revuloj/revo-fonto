    <trd>majusku&lstroke;a</trd>,
    <trd>wersalik</trd>,
    <trd><ind>litera</ind> drukowana<klr> (pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_p;&c_i;&c_s;&c_n;&c_a;&c_ja;
    &c_b;&c_u;&c_k;&c_v;&c_a;</trd>,
    <trd>&c_b;&c_o;&c_l;&c_mol;&c_sh;&c_a;&c_ja;
    &c_b;&c_u;&c_k;&c_v;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_l;&c_a;&c_v;&c_n;&c_a;&c_ja;
    &c_b;&c_u;&c_k;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="tr">b&uuml;y&uuml;k harf</trd>
  <trdgrp lng="uk">
    <trd>&#x456;&#x43D;&#x456;&#x446;&#x456;&#x430;&#x43B;</trd>,
