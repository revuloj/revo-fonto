  <trd lng="it">lettera<klr> (alfabeto)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x6587;&#x5B57;  <pr>&#x3082;&#x3058;</pr></trd>,
    <trd>&#x5B57;  <pr>&#x3042;&#x3056;&#x306A;</pr></trd>,
