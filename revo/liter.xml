  <trd lng="ru">&c_b;&c_u;&c_k;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="liter.malgranda0o">
  <kap>malgranda <tld/>o,
  <var><kap>et<tld/>o</kap></var></kap>
  <snc mrk="liter.malgranda0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref tip="super" cel="liter.0o.propra">Litero</ref> apartenanta
      al la pli ordinara speco ol la
      <ref cel="liter.granda0o">grandaj <tld/>oj</ref>; en Esperanto,
      ano de la serio <em>a, b, c, &ccirc; ...</em>:
      <ekz>
        la &ccirc;iama skribado de &leftquot;vi&rightquot; per malgranda litero estas ne sole tute regula, sed mi
        mem anka&ubreve; nun provos iom post iom forlasi mian &gcirc;isnunan manieron de skribado
        <fnt>
          <bib>LR</bib>
          <lok>8. Personaj pronomoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la et<tld/>o x i&gcirc;as granda X
        <fnt>
          <vrk><url ref="https://lernu.net/lt/forumo/temo/2029">Iksa problemo en reta ligilo</url></vrk>
          <lok>lernu.net</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="minuskl.0o.TIP">minusklo</ref>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_a;&c_ja;
  <ind>&c_l;&c_ib;&c_t;&c_a;&c_r;&c_a;</ind></trd>
