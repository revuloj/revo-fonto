<art mrk="$Id: leptospir.xml,v 1.3 2022/03/22 18:26:51 revo Exp $">
<kap>
  <rad>leptospir</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="leptospir.0o">
  <kap><tld/>o</kap>
  <snc mrk="leptospir.0o.BAK">
    <uzo tip="fak">BAK</uzo>
    <dif>
      <ref tip="super" cel="spirohxet.0o.BAK">Spiro&hcirc;eto</ref>
      vivanta en akvo en senlumaj lokoj,
      ka&ubreve;zanta <ref tip="vid" cel="leptospiroz.0o.MED">leptospirozon</ref>;
      genro (<trd lng="la">Leptospira</trd>):
      <ekz>
        la <tld/>oj eniras la homan korpon tra la ha&ubreve;taj poroj kaj 
        atakas la renojn kaj la hepaton
        <fnt>
          <aut>C. Bertin</aut>,
          <vrk><url ref="http://www.bertin.biz/index.php?titre=kojpo.htm">
          La kojpo</url></vrk>,
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">leptospira</trd>
  <trd lng="fr">leptospire</trd>
  <trd lng="pl">kr&eogonek;tek</trd>
  <trdgrp lng="ro">
    <trd>leptospira</trd>,
    <trd>spirochetei</trd>
  </trdgrp>
  <trd lng="uk">&#x43B;&#x435;&#x43F;&#x442;&#x43E;&#x441;&#x43F;&#x456;&#x440;&#x430;</trd>
</drv>
</art>

