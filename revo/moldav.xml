<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>moldav</rad>/o
  <ofc>*</ofc>
</kap>

<drv mrk="moldav.0ujo">
  <kap>
    <tld lit="M"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="moldav.0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la orienta <ref tip="malprt"
      cel="euxrop1.E0o.geog">E&ubreve;ropo</ref>
      kun &ccirc;efurbo Ki&scirc;inevo.
      Iama respubliko de <ref tip="malprt"
      cel="sovet.S0io">Sovetio</ref>.
      Teritorio de Respubliko Moldavujo.
      ISO-3166 kodo: MD.
      &Gcirc;i limas kun
      <ref tip="vid" cel="ruman.0ujo.GEOG">Rumanujo</ref>
      kaj <ref tip="vid" cel="ukrain.U0ujo">Ukrainujo</ref>.
    </dif>
    <ref tip="sin" cel="moldavi.0o.GEOG">Moldavio</ref>
    <trdgrp lng="pl">
      <trd>Mo&lstroke;dawia</trd>,
      <trd>Mo&lstroke;dowa</trd>,
      <trd>Republika <ind>Mo&lstroke;dawii</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

