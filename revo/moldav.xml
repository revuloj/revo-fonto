<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>moldav</rad>/o
  <ofc>*</ofc>
</kap>

<drv mrk="moldav.0o">
  <kap><tld/>o</kap>
  <snc mrk="moldav.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="moldav.M0ujo"><tld lit="M"/>ujo</ref>.
    </dif>
  </snc>
  <snc mrk="moldav.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco.
    </dif>
  </snc>
  <trd lng="pt">moldavo <klr tip="ind"> (subst.)</klr></trd>
</drv>

<drv mrk="moldav.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato.
    </dif>
  </snc>
  <trd lng="pt">moldavo <klr tip="ind"> (adj.)</klr></trd>
</drv>

<drv mrk="moldav.M0ujo">
  <kap>
    <tld lit="M"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="moldav.0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la orienta <ref tip="malprt"
      cel="euxrop1.E0o.geog">E&ubreve;ropo</ref>
      kun &ccirc;efurbo Ki&scirc;inevo.
      Iama respubliko de <ref tip="malprt"
      cel="sovet.S0io">Sovetio</ref>.
      Teritorio de Respubliko Moldavujo.
      ISO-3166 kodo: MD.
      &Gcirc;i limas kun
      <ref tip="vid" cel="ruman.R0ujo.GEOG">Rumanujo</ref>
      kaj <ref tip="vid" cel="ukrain.U0ujo">Ukrainujo</ref>.
    </dif>
    <ref tip="sin" cel="moldavi.M0o.GEOG">Moldavio</ref>
    <trd lng="bg">&c_M;&c_o;&c_l;&c_d;&c_o;&c_v;&c_a;</trd>
    <trd lng="de">Moldawien</trd>
    <trd lng="en">Moldova</trd>
    <trdgrp lng="pl">
      <trd>Mo&lstroke;dawia</trd>,
      <trd>Mo&lstroke;dowa</trd>,
      <trd>Republika <ind>Mo&lstroke;dawii</ind></trd>
    </trdgrp>
    <trd lng="pt">Mold&aacute;via</trd>
  </snc>
</drv>

</art>

