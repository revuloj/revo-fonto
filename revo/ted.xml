<drv mrk="ted.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ted.0igxi.PSI">
    <dif>
      Eksenti <tld/>on, malagrablon pro
      tro da&ubreve;rinta afero:
      <ekz>
        Mi instigos kontra&ubreve; vin viajn amistojn, de kiuj via animo <tld/>i&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 23:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        finfine li tedi&gcirc;as pro la moketoj de Sofia
        <fnt>
          <aut>Sten Johansson</aut>,
          <vrk><url
          ref="http://books.google.fr/books?id=-OpuAgAAQBAJ&amp;pg=PA20&amp;lpg=PA20&amp;dq=Sten+Johansson+marina+tedi&gcirc;as&amp;source=bl&amp;ots=Bsn_ZhxUHj&amp;sig=QCniLOzfjlPTA-WHcI_e6KgGrxo&amp;hl=fr&amp;sa=X&amp;ei=KeneVMemAYfxUt2NgrAP&amp;ved=0CE8Q6AEwBQ#v=onepage&amp;q=Sten%20Johansson%20marina%20tedi&gcirc;as&amp;f=false">Marina</url></vrk>,
          <lok>p. 20</lok>
        </fnt>;
      </ekz>
      <ekz>
        la senmajusklaj frazoj estas ofte tiom longaj, ke mi <tld/>i&gcirc;is
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2008/009457.php">Nigra apokalipso</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="enu.0i">enui</ref>
  </snc>
  <trd lng="be">&c_n;&c_u;&c_d;&c_z;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trd lng="de">etwas <ind>&uuml;berdr&uuml;ssig</ind> werden</trd>
  <trdgrp lng="en">
    <trd>to weary</trd>,
    <trd>become bored</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd><ind>cansar</ind>se<klr> (de)</klr></trd>,
    <trd><ind>aburrir</ind>se<klr> (de)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>lasser</ind><klr> (de)</klr></trd>,
    <trd>en avoir <ind>marre</ind><klr> (de)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12358;&#12435;&#12374;&#x308A;&#12377;&#x308B;</trd>,
    <trd>&#x98FD;&#x304D;&#x98FD;&#x304D;&#12377;&#x308B;  <pr>&#12354;&#x304D;&#12354;&#x304D;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="pl">nudzi&cacute; si&eogonek;</trd>
  <trdgrp lng="zh">
    <trd>&#x538C;&#20518; <pr>y&#xE0;nju&#xE0;n</pr></trd>,
    <trd>&#x53AD;&#20518; <pr>y&#xE0;nju&#xE0;n</pr></trd>
  </trdgrp>
</drv>
