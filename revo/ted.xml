<drv mrk="ted.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ted.0i.PSI">
    <dif>
      Ka&ubreve;zi lacecon kaj malagrablecon pro troa da&ubreve;ro,
      ripeti&gcirc;o a&ubreve; abundo:
      <ekz>
        jam <tld/>as al mi a&ubreve;dadi pri li
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        en juneco logas, en maljuneco <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        rabista knabino, al kiu <tld/>is sidi hejme kaj kiu volis rajdi <klr>[&#8230;]</klr>,
        kien &scirc;ia kaprico &scirc;in pelos
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i na&gcirc;is kaj na&gcirc;is, kaj fine tiu na&gcirc;ado tute <tld/>is &gcirc;in
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ar &scirc;i <tld/>adis lin per siaj vortoj &ccirc;iutage kaj turmentadis lin, lia animo
        morte laci&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 16:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        la penta vivo de la templo <tld/>is lin, li eksopiris al festenoj, virinoj kaj
        bruo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="enu.0igi">enuigi</ref>
    <refgrp tip="vid">
      <ref cel="cxerp.el0i.cxion">el&ccirc;erpi<sncref/></ref>,
      <ref cel="frustr.0i">frustri</ref>,
      <ref cel="gxen.0i">&gcirc;eni</ref>,
      <ref cel="incit.0i">inciti</ref>,
      <ref cel="lac.0igi.KOMUNE">lacigi</ref>,
      <ref cel="nauxz.0i">na&ubreve;zi</ref>,
      <ref cel="turmen.0i">turmenti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="ja">
    <trd>&#12358;&#12435;&#12374;&#x308A;&#12373;&#x305B;&#x308B;</trd>,
    <trd>&#x304F;&#12393;&#x304F;&#12393;&#x8A00;&#12387;&#12390;&#x56F0;&#12425;&#x305B;&#x308B; 
    <pr>&#x304F;&#12393;&#x304F;&#12393;&#12356;&#12387;&#12390;&#12371;&#x307E;&#12425;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x98FD;&#x304D;&#x98FD;&#x304D;&#12373;&#x305B;&#x308B; 
