<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ted</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ted.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ted.0i.PSI">
    <dif>
      Ka&ubreve;zi lacecon kaj malagrablecon pro troda&ubreve;ro,
      troripeti&gcirc;o a&ubreve; troabundo:
      <ekz>
        la sen&ccirc;esaj pluvoj <tld/>is la voja&gcirc;antojn
        <fnt>B</fnt>;
      </ekz>
      <ekz>
        jam <tld/>as al mi a&ubreve;dadi pri li<fnt>Z</fnt>;
      </ekz>
      <ekz>
        esti <tld/>ita de io<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="enu.0igi">enuigi</ref>,
      <ref cel="incit.0i">inciti</ref>,
      <ref cel="lac.0igi.KOMUNE">lacigi</ref>,
      <ref cel="nauxz.0i">na&ubreve;zi</ref>,
      <ref cel="turmen.0i">turmenti</ref>
    </refgrp>
  </snc>
  <trd lng="lat">taedet, ere<klr>aliquem alicuius rei</klr></trd>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_d;&c_a;&c_k;&c_u;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_a;&c_k;&c_u;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_r;&c_y;&c_d;&c_z;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nudit</trd>,
    <trd>obtěžovat</trd>,
    <trd>omrzet</trd>,
    <trd>otravovat</trd>,
    <trd>unavovat <klr>(někoho)</klr></trd>,
    <trd>znechutit</trd>
  </trdgrp>
  <trd lng="de">langweilen</trd>
  <trd lng="en">bore</trd>
  <trdgrp lng="es">
    <trd>aburrir</trd>,
    <trd>fastidiar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ennuyer</trd>,
    <trd>lasser</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>untat</trd>,
    <trd>f&aacute;raszt</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vervelen</trd>,
    <trd>vermoeien</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>nudzi&cacute;</trd>,
    <trd>zanudza&cacute;</trd>,
    <trd>nu&zdot;y&cacute;</trd>,
    <trd>dokucza&cacute;</trd>,
    <trd>naprzykrza&cacute; si&eogonek;</trd>,
    <trd>uprzykrza&cacute;</trd>,
    <trd>niepokoi&cacute;</trd>,
    <trd>obrzydza&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_d;&c_o;&c_je;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_o;&c_je;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nudiť</trd>,
    <trd>otravovať</trd>,
    <trd>sprotiviť</trd>,
    <trd>unúvať</trd>,
    <trd>znechutiť</trd>
  </trdgrp>
</drv>

<drv mrk="ted.0a">
  <kap><tld/>a</kap>
  <snc mrk="ted.0a.PSI">
    <dif>
      Kiu ka&ubreve;zas <tld/>on, lacan inciti&gcirc;on
      pro da&ubreve;ro, ripeti&gcirc;o, unutoneco:
      <ekz>
        <tld/>a kiel kulo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>bezduchý</trd>,
    <trd>nudný</trd>,
    <trd>únavný</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ennuyeux</trd>,
    <trd>lassant</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nezáživný</trd>,
    <trd>nudný</trd>
  </trdgrp>
</drv>

<drv mrk="ted.0o">
  <kap><tld/>o</kap>
  <snc mrk="ted.0o.PSI">
    <dif>
      Sento de tiu, kiun io <tld/>as:
      <ekz>
        kontra&ubreve; &ccirc;iu <tld/>o ekzistas rimedo
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>nuda</trd>,
    <trd>únava</trd>
  </trdgrp>
  <trd lng="fr">lassitude</trd>
  <trdgrp lng="sk">
    <trd>protiveň</trd>,
    <trd>unúvanie</trd>,
    <trd>znechutenie</trd>
  </trdgrp>
</drv>

<drv mrk="ted.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ted.0igxi.PSI">
    <dif>
      Eksenti <tld/>on, malagrablon pro
      tro da&ubreve;rinta afero:
      <ekz>
        mia animo <tld/>i&gcirc;is de &scirc;i<fnt>Z</fnt>;
      </ekz>
      <ekz>
        finfine li tedi&gcirc;as pro la moketoj de Sofia
        <fnt>
          <aut>Sten Johansson</aut>,
          <vrk><url
          ref="http://books.google.fr/books?id=-OpuAgAAQBAJ&amp;pg=PA20&amp;lpg=PA20&amp;dq=Sten+Johansson+marina+tedi&gcirc;as&amp;source=bl&amp;ots=Bsn_ZhxUHj&amp;sig=QCniLOzfjlPTA-WHcI_e6KgGrxo&amp;hl=fr&amp;sa=X&amp;ei=KeneVMemAYfxUt2NgrAP&amp;ved=0CE8Q6AEwBQ#v=onepage&amp;q=Sten%20Johansson%20marina%20tedi&gcirc;as&amp;f=false">Marina</url></vrk>,
          <lok>p. 20</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>se <ind>lasser</ind><klr> (de)</klr></trd>,
    <trd>en avoir <ind>marre</ind><klr> (de)</klr></trd>
  </trdgrp>
</drv>
</art>
