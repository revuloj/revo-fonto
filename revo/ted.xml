<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ted</rad>/i
</kap>
<drv mrk="ted.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ka&ubreve;zi lacecon kaj malagrablecon pro troda&ubreve;ro,
      troripeti&gcirc;o a&ubreve; troabundo:
      <ekz>
        la sen&ccirc;esaj pluvoj <tld/>is la voja&gcirc;antojn
        <fnt>B</fnt>;
      </ekz>
      <ekz>
        vi <tld/>us lin<fnt>Z</fnt>;
      </ekz>
      <ekz>
        jam <tld/>as al mi a&ubreve;dadi pri li<fnt>Z</fnt>;
      </ekz>
      <ekz>
        esti <tld/>ita de io<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mia animo <tld/>i&gcirc;is de &scirc;i<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kontra&ubreve; &ccirc;iu <tld/>o ekzistas rimedo
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>a kiel kulo.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="nauxz.0i">na&ubreve;zi</ref>,
      <ref cel="lac">lacigi</ref>,
      <ref cel="enu.0igi">enuigi</ref>,
      <ref cel="sat">supersatigi</ref>,
      <ref cel="cxagre.0i">&ccirc;agreni</ref>,
      <ref cel="turmen.0i">turmenti</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_d;&c_a;&c_k;&c_u;&c_ch;&c_v;&c_a;&c_c;&c_mol;
    </trd>,
    <trd>&c_n;&c_a;&c_d;&c_a;&c_k;&c_u;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_r;&c_y;&c_d;&c_z;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">langweilen</trd>
  <trd lng="en">bore</trd>
  <trdgrp lng="fr">
    <trd>ennuyer</trd>, <trd>lasser</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>aburrir</trd>,
    <trd>fastidiar</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>untat</trd>,
    <trd>f&aacute;raszt</trd>
  </trdgrp>
  <trd lng="lat">taedet, ere<klr>aliquem alicuius rei</klr></trd>
  <trdgrp lng="nl">
    <trd>vervelen</trd>,
    <trd>vermoeien</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>nudzi&cacute;</trd>,
    <trd>zanudza&cacute;</trd>,
    <trd>nu&zdot;y&cacute;</trd>,
    <trd>dokucza&cacute;</trd>,
    <trd>naprzykrza&cacute; si&eogonek;</trd>,
    <trd>uprzykrza&cacute;</trd>,
    <trd>niepokoi&cacute;</trd>,
    <trd>obrzydza&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_d;&c_o;&c_je;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_o;&c_je;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
