<kap>
  <rad>litargir</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="litarg.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Protoksido de plumbo de oran&gcirc;eru&gcirc;a koloro, fandata kaj lamenforme
      kristali&gcirc;anta:
      <ekz>
        <tld/>o estas uzata en preparado de glazuro.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_l;&c_jo;&c_t; &c_s;&c_mol;&c_v;&c_ib;&c_n;&c_c;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_a;&c_k;&c_s;&c_ib;&c_d; <ind>&c_s;&c_mol;&c_v;&c_ib;&c_n;&c_c;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="bg">&c_o;&c_l;&c_o;&c_v;&c_je;&c_n; &c_o;&c_k;&c_i;&c_s;</trd>
  <trdgrp lng="de">
     <trd>Bleigl&auml;tte</trd>,
     <trd>Bleimonoxid</trd>,
     <trd>Bleioxid</trd>
  </trdgrp>
  <trd lng="en">litharge</trd>
  <trd lng="es">litargirio</trd>
  <trd lng="fr">litharge</trd>
  <trd lng="hu">&oacute;lomgl&eacute;t</trd>
  <trdgrp lng="ja">
    <trd>&#x30EA;&#x30B5;&#x30FC;&#x30B8;</trd>,
    <trd>&#x5BC6;&#x9640;&#x50E7;  <pr>&#x307F;&#x3064;&#x3060;&#x305D;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">loodglit</trd>
  <trdgrp lng="pl">
    <trd>glejta</trd>,
    <trd>tlenek o&lstroke;owiu</trd>
  </trdgrp>
  <trd lng="pt">litarg&iacute;rio</trd>
  <trd lng="ro">oxid de plumb</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_v;&c_i;&c_n;&c_c;&c_o;&c_v;&c_y;&c_j; &c_g;&c_l;&c_je;&c_t;</trd>,
    <trd>&c_o;&c_k;&c_i;&c_s;&c_mol; &c_s;&c_v;&c_i;&c_n;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_v;&c_i;&#x43D;&c_c;&c_je;&c_v;&c_i;&c_j; &c_g;&#x43B;&c_je;&c_t;</trd>,
    <trd>&#x43E;&#x43A;&c_i;&c_s; &c_s;&c_v;&c_i;&#x43D;&c_c;&#x44E;</trd>
  </trdgrp>
</drv>
</art>
