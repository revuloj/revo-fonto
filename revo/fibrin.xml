    </dif>
    <trdgrp lng="hu">
      <trd>fibrin</trd>,
      <trd>rostanyag</trd>
    </trdgrp>
    <trd lng="pl">w&lstroke;&oacute;knik</trd>
    <trdgrp lng="pt">
      <trd>fibrina vegetal</trd>
    </trdgrp>
  </snc>
</drv>
-->
</art>
