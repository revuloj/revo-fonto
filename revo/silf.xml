<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>silf</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="silf.0o">
  <kap><tld/>o</kap>
  <snc mrk="silf.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Aera, fluga <ref tip="super" cel="fe.0o.MIT">feo</ref>:
      <ekz>
        malfermi&gcirc;is pordo, la vento kaptis la dancistinon, kaj simile al
        <tld/>ino &scirc;i <klr>...</klr> flugis <klr>...</klr> kaj malaperis
        <fnt><bib>Fab1</bib>, <lok>persista stana soldato</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">s&iacute;lfide</trd>
  <trd lng="es">s&iacute;lfide</trd>
  <trd lng="fr">sylphe</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_i;&c_l;&c_mol;&c_f;</trd>,
    <trd>&c_s;&c_i;&c_l;&c_mol;&c_f;&c_i;&c_d;&c_a;</trd>
  </trdgrp>
</drv>
</art>

