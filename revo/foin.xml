<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>foin</rad>/o
</kap>
<drv mrk="foin.0o">
  <kap><tld/>o</kap>
  <snc mrk="foin.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Specio de <ref tip="super" cel="martes.0o.ZOO_genro">marteso</ref>
      (<trd lng="la">Martes foina</trd>) el E&ubreve;ropo kaj Norda Azio, kun
      duparta blanka brustmakulo.
    </dif>
  </snc>
  <trd lng="cs">kuna skaln&iacute;</trd>
  <trd lng="de">Steinmarder</trd>
  <trd lng="fr">fouine</trd>
  <trd lng="it">faina</trd>
  <trd lng="nl">steenmarter</trd>
  <trd lng="hu">nyest</trd>
  <trdgrp lng="pt">
    <trd>fuinha</trd>,
    <trd>fuinho</trd>,
    <trd>gardunho</trd>,
    <trd>gardunho</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_n;&c_n;&c_a;&c_ja; &c_k;&c_u;&c_n;&c_i;&c_c;&c_a;</trd>
</drv>
</art>
