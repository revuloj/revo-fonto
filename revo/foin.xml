<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: foin.xml,v 1.20 2022/03/20 11:57:21 revo Exp $">
<kap>
  <rad>foin</rad>/o
</kap>
<drv mrk="foin.0o">
  <kap><tld/>o</kap>
  <snc mrk="foin.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:zoologiaj_specioj">Specio</ref> de
      <ref tip="super" cel="martes.0o.ZOO_genro">marteso</ref>
      (<trd lng="la">Martes foina</trd>),
      malgranda <ref tip="lst" cel="best.rabo0o" lst="voko:rabobestoj">rabobesto</ref> el E&ubreve;ropo kaj Norda Azio,
      kun duparta blanka brustmakulo, konata pri tio, ke &gcirc;i &ccirc;asas anka&ubreve;
      kortbirdojn kaj dismordas gumajn partojn de a&ubreve;toj:
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> havas brilan blugrizan &gcirc;is blubrunan hararon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=9356">Foino</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_je;&c_n;&c_n;&c_a;&c_ja; <ind>&c_k;&c_u;&c_n;&c_ib;&c_c;&c_a;</ind></trd>
  <trd lng="bg">&c_b;&c_je;&c_l;&c_k;&c_a;</trd>
  <trd lng="ca">fagina</trd>
  <trdgrp lng="cs">
    <trd>kuna dom&aacute;c&iacute;</trd>,
    <trd>kuna skaln&iacute;</trd>
  </trdgrp>
  <trd lng="da">husm&aring;r</trd>
  <trd lng="de">Steinmarder</trd>
  <trdgrp lng="en">
    <trd>beech <ind>marten</ind></trd>,
    <trd>stone <ind>marten</ind></trd>
  </trdgrp>
  <trd lng="fa">&a_s;&a_m;&a_w;&a_r; &a_s;&a_n;&f_g;&f_y;</trd>
  <trd lng="fr">fouine</trd>
  <trd lng="hu">nyest</trd>
  <trd lng="it">faina</trd>
  <trd lng="nl">steenmarter</trd>
  <trdgrp lng="pl">
    <trd>kuna domowa</trd>,
    <trd>kamionka <klr>(pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fuinha</trd>,
    <trd>fuinho</trd>,
    <trd>gardunho</trd>,
    <trd>gardunho</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_n;&c_n;&c_a;&c_ja; &c_k;&c_u;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">kuna skaln&aacute;</trd>
  <trd lng="sv">stenm&aring;rd</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x443;&#x43D;&#x438;&#x446;&#x44F; &#x43A;&#x430;&#x43C;&#x2019;&#x44F;&#x43D;&#x430;</trd>,
    <trd>&#x43A;&#x443;&#x43D;&#x438;&#x446;&#x44F; &#x445;&#x430;&#x442;&#x43D;&#x44F;</trd>,
    <trd>&#x431;&#x456;&#x43B;&#x43E;&#x434;&#x443;&#x448;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>
</art>
