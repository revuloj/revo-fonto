<art mrk="$Id$">
<kap>
<rad>Florenc</rad>/o
</kap>
<drv mrk="florenc.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      &Ccirc;efurbo de <ref tip="vid" cel="toskan.0ujo">Toskanujo</ref>.
