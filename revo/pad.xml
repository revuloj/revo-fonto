<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>pad</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="pad.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">TRA</uzo>
    <dif>
      Eta kampara <ref tip="super" cel="voj.0o.TRA">vojo</ref> sen tega&jcirc;o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>o de la virto stretas
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 6a, p.
        427a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="ale.0o.HOR">aleo <sncref/></ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_c;&c_je;&c_zh;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_mol;&c_c;&c_ja;&c_zh;&c_y;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Pfad</trd>
  <trdgrp lng="es">
    <trd>sendero</trd>,
    <trd>senda</trd>,
    <trd>camino<klr> (en campo, bosque, mota&ntilde;a...)</klr></trd>,
  </trdgrp>
  <trdgrp lng="fr">
    <trd>chemin<klr> (sentier)</klr></trd>,
    <trd>sentier</trd>
  </trdgrp>
  <trd lng="hu">ouml;sv&eacute;ny</trd>
  <trd lng="pt">trilha</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_o;&c_p;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_o;&c_p;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_o;&c_p;&c_i;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
