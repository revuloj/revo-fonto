<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>pad</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="pad.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TRA</uzo>
  <snc>
    <dif>
      Eta kampara <ref tip="super" cel="voj.0o.TRA">vojo</ref> sen tega&jcirc;o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la pado de la virto stretas
        <fnt><bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 6a, p.
        427a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="ale.0o.HOR">aleo <sncref/></ref>
  </snc>
  <trdgrp lng="fr">
    <trd>chemin<klr> (sentier)</klr></trd>,
    <trd>sentier</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_o;&c_p;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_o;&c_p;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_o;&c_p;&c_i;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>

