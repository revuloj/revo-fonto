<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pad.xml,v 1.22 2023/10/20 14:25:19 revo Exp $">
<kap>
  <rad>pad</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="pad.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">TRA</uzo>
    <dif>
      <ref tip="super" cel="voj.0eto">Vojeto</ref> sur natura grundo
      esti&gcirc;inta pro ofta pasado de piedirantoj, rajdbestoj a&ubreve; bicikloj, sen tega&jcirc;o:
      <ekz>
        forlasinte la <tld/>on, li tra la olivarba densejo iris suden
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        nun ni forlasis e&ccirc; tiun a&ccirc;an vojon kaj devoji&gcirc;is sur herbejan <tld/>on
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>o de la virto stretas
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 6a, p.
        427a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="ale.0o.HOR">aleo<sncref/></ref>
    <bld lok="&WCU;/thumb/f/fa/Wanderung_auf_dem_Alpinen_Pfad_am_Feldberg%2C_April_2020_66.jpg/270px-Wanderung_auf_dem_Alpinen_Pfad_am_Feldberg%2C_April_2020_66.jpg" prm="CC BY-SA 4.0">montara <tld/>o en Nigra Arbaro, Germanujo
        <fnt>
          <aut>Michael Schmalenstroer, propra verko</aut>
          <lok><url ref="&WCW;/File:Wanderung_auf_dem_Alpinen_Pfad_am_Feldberg,_April_2020_66.jpg">&WCW;/File:Wanderung_auf_dem_Alpinen_Pfad_am_Feldberg,_April_2020_66.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <ref tip="hom" cel="pad1.0o">Pado (itala rivero)</ref>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_c;&c_je;&c_zh;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_mol;&c_c;&c_ja;&c_zh;&c_y;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_p;&c_malmol;&c_t;&c_je;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_malmol;&c_t;</trd>
  </trdgrp>
  <trd lng="bo">&#xF63;&#xF58;&#xF0B;&#xF41;&#xF42;&#xF0B;</trd>
  <trd lng="cs">stezka</trd>
  <trd lng="de">Pfad</trd>
  <trd lng="en">path</trd>
  <trdgrp lng="es">
    <trd>sendero</trd>,
    <trd>senda</trd>,
    <trd>camino<klr> (en campo, bosque, mota&ntilde;a...)</klr></trd>,
  </trdgrp>
  <trdgrp lng="fr">
    <trd>chemin<klr> (sentier)</klr></trd>,
    <trd>sentier</trd>
  </trdgrp>
  <trd lng="hu">&ouml;sv&eacute;ny</trd>
  <trdgrp lng="ja">
    <trd>&#x5C0F;&#x9053;  <pr>&#x3053;&#x307F;&#x3061;</pr></trd>,
    <trd>&#x7D30;&#x9053;  <pr>&#x307B;&#x305D;&#x307F;&#x3061;</pr></trd>
  </trdgrp>
  <trd lng="ne">&#x92C;&#x93E;&#x91F;&#x94B;</trd>
  <trd lng="nl">pad<klr tip="ind"> (weg)</klr></trd>
  <trdgrp lng="pl">
    <trd>dr&oacute;&zdot;ka</trd>,
    <trd>&sacute;cie&zdot;ka</trd>
  </trdgrp>
  <trd lng="pt">trilha</trd>
  <trdgrp lng="ro">
    <trd>c&abreve;rare</trd>,
    <trd>potec&abreve;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_o;&c_p;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_o;&c_p;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_o;&c_p;&c_i;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chodn&iacute;&ccaron;ek</trd>,
    <trd>pr&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_je;&c_zh;&c_i;&#x43D;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_je;&c_zh;&#x43A;&c_a;</trd>,
    <trd>&c_d;&#x43E;&c_r;&c_ib;&c_zh;&#x43A;&c_a;</trd>
  </trdgrp>
</drv>
</art>
