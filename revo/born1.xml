<vortaro>
  <art mrk="$Id: born1.xml,v 1.3 2024/10/10 13:35:45 afido Exp $">
    <kap>
      <rad>Born</rad>/o
    </kap>

<drv mrk="born1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc mrk="born1.0o.subsxtato">
    <dif>
      Sub&scirc;tato en la nordoriento de <ref tip="malprt" cel="nigxeri.N0o">Ni&gcirc;erio</ref>:
      <ekz>
        akri&gcirc;is la atakoj aparte en &scirc;tato <tld/>o, kiu dividas landlimon kun &Ccirc;ado
        <fnt>
          <bib>Monato</bib>
          <aut>Princo Henriko Oguinye</aut>
          <vrk><url ref="&Monato;/2009/009885.php"
              >Mortigoj religiaj</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la &scirc;tato <tld/>o la radikala sekto <nom>Boko Haram</nom> atakis policistojn kaj soldatojn
        <fnt>
          <bib>Monato</bib>
          <aut>Princo Henriko Oguinye</aut>
          <vrk><url ref="&Monato;/2011/010274.php"
              >Malfeli&ccirc;an kristnaskon</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Borno</trd>
    <trd lng="en">Borno state</trd>
    <trd lng="fr">&eacute;tat de Borno</trd>
    <trd lng="pl">Borno</trd>
  </snc>
  <ref tip="hom" cel="born.0o">borno (klemo)</ref>
<trdgrp lng="be">
  <trd>&c_B;&c_o;&c_r;&c_n;&c_a; <klr>(&c_sh;&c_t;&c_a;&c_t; &c_u; &c_N;&c_ib;&c_g;&c_je;&c_r;&c_y;&c_ib;)</klr></trd>,
  <trd>&c_B;&c_o;&c_r;&c_n;&c_u;</trd>
</trdgrp>
</drv>
  </art>

