<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: herd.xml,v 1.4 2023/02/23 09:29:15 revo Exp $">
<kap>
  <rad>herd</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="herd.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="herd.0o.ZOO">
    <dif>
      Granda <ref cel="greg.0o.AGR">grego</ref>
      de bestoj, precipe de hufuloj:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la razkapuloj movi&gcirc;is la&ubreve;eble en <tld/>oj, kaj
        atakis prefere gejojn, pakistananojn kaj maljunulinojn
        <fnt>
          <aut>T. Steele</aut>
          <vrk>Konvinka Kamufla&jcirc;o</vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_t;&c_a;&c_k;</trd>
  <trd lng="de">Herde</trd>
  <trd lng="en">herd</trd>
  <trd lng="fr">harde</trd>
  <trd lng="uk">&c_s;&c_t;&c_a;&c_d;&#x43E;</trd>
</drv>
</art>

