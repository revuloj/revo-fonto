<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>specif</rad>/i
      <fnt>Z</fnt>
    </kap>

    <drv mrk="specif.0i">
      <kap><tld/>i</kap>
      <snc>
        <dif>
          Precize kaj detale difini:
          <ekz>
            <tld/>i la reciprokajn rajtojn en kontrakto.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
       
<trd>&c_s;&c_p;&c_e;&c_c;&c_y;&c_f;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_v;&c_y;&c_z;&c_n;&c_a;&c_ch;&c_a;&c_c;&c_mol;</trd>,
       
<trd>&c_u;&c_d;&c_a;&c_k;&c_l;&c_a;&c_d;&c_n;&c_ja;&c_c;&c_mol;</trd>
      </trdgrp>
      <trd lng="de">spezifizieren</trd>
      <trdgrp lng="hu">
        <trd>specifik&aacute;l</trd>,
        <trd>r&eacute;szletesen <ind>meghat&aacute;roz</ind></trd>
      </trdgrp>
      <trd lng="nl">specificeren</trd>
      <trdgrp lng="ru">
       
<trd>&c_s;&c_p;&c_je;&c_c;&c_i;&c_f;&c_i;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_t;&c_o;&c_ch;&c_n;&c_o;
<ind>&c_o;&c_p;&c_r;&c_je;&c_d;&c_je;&c_l;&c_ja;&c_t;&c_mol;</ind></trd>,
        <trd>&c_u;&c_t;&c_o;&c_ch;&c_n;&c_ja;&c_t;&c_mol;</trd>
      </trdgrp>
    </drv>

    <drv mrk="specif.0a">
      <kap><tld/>a</kap>
      <snc mrk="specif.0a.aparta">
        <dif>
          Havanta speciale difinitan karakteron.
        </dif>
        <trdgrp lng="ru">
         
<trd>&c_s;&c_p;&c_je;&c_c;&c_i;&c_f;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
         
<trd>&c_s;&c_p;&c_je;&c_c;&c_i;&c_f;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
        </trdgrp>
      </snc>
      <snc mrk="specif.0a.FIZ">
        <uzo tip="fak">FIZ</uzo>
        <dif>
          <ref tip="super" cel="po.0a">Poa</ref>, indikita rilate
          al mezurunuo, precipe tiu de
          <refgrp>
            <ref cel="volume.0o.MAT">volumeno</ref>,
            <ref cel="mas.0o.FIZ">maso</ref>,
            <ref cel="energi.0o.FIZ">energio</ref>
          </refgrp>:
          <ekz>
            <ind><tld/>a pezo</ind>
            <trd lng="ru">&c_u;&c_d;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;
&c_v;&c_je;&c_s;</trd>
          </ekz><ekz>
            <ind><tld/>a volumeno</ind>.
            <trd lng="ru">&c_u;&c_d;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;
&c_o;&c_b;&c_malmol;&c_jo;&c_m;</trd>
          </ekz>
        </dif>
        <rim>
          <ctl>Poa</ctl> estas tiusence pli eleganta kaj esperanteca,
          dum &leftquot;<tld/>a&rightquot; estas senlogika idioma&jcirc;o,
kvankam ja komuna al
          la lingvoj okcidentaj.
          <aut>Sergio Pokrovskij</aut>
        </rim>
        <trd lng="ru">&c_u;&c_d;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
      </snc>
      <trd
lng="be">&c_s;&c_p;&c_e;&c_c;&c_y;&c_f;&c_ib;&c_ch;&c_n;&c_y;</trd>
      <trd lng="de">spezifisch</trd>
      <trdgrp lng="hu">
        <trd>specifikus</trd>,
        <trd>saj&aacute;tos</trd>
      </trdgrp>
      <trd lng="en">specific</trd>
      <trd lng="nl">specifiek</trd>
    </drv>
  </art>
