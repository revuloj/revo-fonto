<vortaro>
<art mrk="$Id$">
<kap><rad>gang</rad>/o</kap>
<drv mrk="gang.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOL</uzo>
  <snc>
    <dif>
      Tereca maso, kiu envolvas mineralon, multekostan &scirc;tonon.
    </dif>
    <refgrp tip="vid">
      <ref cel="geod.0o">geodo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_zh;&c_y;&c_l;&c_a;</trd>
  <trd lng="ca">ganga</trd>
  <trdgrp lng="de">
    <trd>Erzader</trd>,
    <trd>Erzgang</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>lode</trd>,
    <trd>vein</trd>
  </trdgrp>
  <trd lng="es">ganga</trd>
  <trd lng="fr">gangue</trd>
  <trdgrp lng="hu">
    <trd>tel&eacute;r</trd>,
    <trd>&eacute;rc&eacute;r</trd>
  </trdgrp>
  <trd lng="nl">ganggesteente</trd>
  <trd lng="pt">ganga</trd>
  <trd lng="ru">&c_zh;&c_i;&c_l;&c_a;</trd>
</drv>
</art>
