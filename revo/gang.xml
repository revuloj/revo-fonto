<art mrk="$Id: gang.xml,v 1.21 2024/09/30 21:28:49 revo Exp $">
  <kap><rad>gang</rad>/o
  <fnt><bib>PV</bib></fnt>
  </kap>
<drv mrk="gang.0o">
  <kap><tld/>o
  </kap>
  <uzo tip="fak">GEOL</uzo>
  <snc>
    <dif>
      Tereca maso, kiu envolvas <ref tip="prt" cel="minera.0o">mineralo</ref>n, multekostan &scirc;tonon:
      <ekz>
        proksime de vila&gcirc;o trovi&gcirc;as la plej konata en Pollando <tld/>o de agatoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=258952">P&lstroke;&oacute;czki G&oacute;rne</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="min.0o.MIN">mino<sncref/></ref>,
      <ref cel="erc.0o">erco</ref>,
      <ref cel="geod.0o">geodo</ref>
    </refgrp>
  </snc>
  <rim>
     <ref tip="hom" cel="gang1.0o">Gango</ref> estas anka&ubreve; granda rivero en Hindujo.
  </rim>
  <trd lng="be">&c_zh;&c_y;&c_l;&c_a;</trd>
  <trd lng="ca">ganga</trd>
  <trdgrp lng="de">
    <trd>Erzader</trd>,
    <trd>Erzgang</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>lode</trd>,
    <trd>vein</trd>
  </trdgrp>
  <trd lng="es">ganga</trd>
  <trd lng="fr">gangue</trd>
  <trdgrp lng="hu">
    <trd>tel&eacute;r</trd>,
    <trd>&eacute;rc&eacute;r</trd>
  </trdgrp>
  <trd lng="ja">&#33032;&#x77F3;  <pr>&#x307F;&#12419;&#x304F;&#x305B;&#x304D;</pr></trd>
  <trd lng="nl">ganggesteente</trd>
  <trd lng="pt">ganga</trd>
  <trd lng="ru">&c_zh;&c_i;&c_l;&c_a;</trd>
  <trd lng="uk">&c_r;&c_u;&c_d;&#x43D;&c_a; &c_zh;&c_i;&#x43B;&c_a;</trd>
  <trd lng="zh">&#x77FF;&#33033; <pr>ku&#xE0;ngm&#xE0;i</pr></trd>
</drv>
</art>
