<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ada&gcirc;</rad>/o
</kap>
<drv mrk="adagx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Muzikverko, kies movo estas malrapida.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_a;&c_zh;&c_y;&c_jo;</trd>
  <trd lng="de">Adagio</trd>
  <trd lng="es">adagio</trd>
  <trd lng="fa">&a_A_madda;&a_d;&a_A;&a_j;&f_y;&a_w;</trd>
  <trd lng="fr">adagio<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="hu">adagio</trd>
  <trd lng="it">adagio<klr> (mus.)</klr></trd>
  <trd lng="nl">adagio</trd>
  <trd lng="ru">&c_a;&c_d;&c_a;&c_zh;&c_i;&c_o;</trd>
  <trd lng="sk">adagio</trd>
</drv>

<drv mrk="adagx.0e">
  <kap><tld/>e</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Malrapide.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_a;&c_zh;&c_y;&c_jo;</trd>
  <trd lng="de">adagio</trd>
  <trd lng="en">adagio</trd>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_r;&a_A;&a_m;<klr tip="ind"> (&a_m;&a_w;&a_s;&f_y;&a_q;&f_y;)</klr></trd>,
    <trd>&a_A_madda;&a_d;&a_A;&a_j;&f_y;&a_w;</trd>
  </trdgrp>
  <trd lng="fr">adagio<klr tip="ind"> (adv.)</klr></trd>
  <trd lng="hu">adagio</trd>
  <trd lng="it">adagio<klr> (mus.)</klr></trd>
  <trd lng="nl">adagio</trd>
  <trd lng="sk">adagio <klr>(pomaly, v hudbe)</klr></trd>
</drv>
</art>
