<kap>
  <rad>ada&gcirc;</rad>/o
</kap>
<drv mrk="adagx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Muzikverko, kies movo estas malrapida:
      <ekz>
        &ccirc;u tio &ccirc;i efektive povas esti varia&jcirc;o el la sama temo, kiu estigas la <tld/>on n-ro 25
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=32931">Goldberg-variacioj</url></lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_a;&c_zh;&c_y;&c_jo;</trd>
  <trd lng="ca">adagio</trd>
  <trd lng="de">Adagio</trd>
  <trd lng="es">adagio</trd>
  <trd lng="fa">&a_A_madda;&a_d;&a_A;&a_j;&f_y;&a_w;</trd>
  <trd lng="fr">adagio<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="hu">adagio</trd>
  <trd lng="it">adagio<klr> (mus.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x30A2;&#x30C0;&#x30FC;&#x30B8;&#x30E7;</trd>,
    <trd>&#x7DE9;&#x5F90;&#x66F2;  <pr>&#x304B;&#x3093;&#x3058;&#x3087;&#x304D;&#x3087;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">adagio</trd>
  <trd lng="pl">adagio</trd>
  <trd lng="ro">adagio</trd>
  <trd lng="ru">&c_a;&c_d;&c_a;&c_zh;&c_i;&c_o;</trd>
  <trd lng="sk">adagio</trd>
  <trdgrp lng="uk">
    <trd>&#x430;&#x434;&#x430;&#x436;&#x456;&#x43E;</trd>,
    <trd>&#x430;&#x434;&#x430;&#x434;&#x436;&#x43E;</trd>
  </trdgrp>
</drv>

<drv mrk="adagx.0e">
  <kap><tld/>e</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Malrapide.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_a;&c_zh;&c_y;&c_jo;</trd>
  <trd lng="ca">adagio</trd>
  <trd lng="de">adagio</trd>
  <trd lng="en">adagio</trd>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_r;&a_A;&a_m;<klr tip="ind"> (&a_m;&a_w;&a_s;&f_y;&a_q;&f_y;)</klr></trd>,
    <trd>&a_A_madda;&a_d;&a_A;&a_j;&f_y;&a_w;</trd>
  </trdgrp>
  <trd lng="fr">adagio<klr tip="ind"> (adv.)</klr></trd>
  <trd lng="hu">adagio</trd>
  <trd lng="it">adagio<klr> (mus.)</klr></trd>
  <trd lng="ja">&#x3086;&#x308B;&#x3084;&#x304B;&#x306B;</trd>
  <trd lng="nl">adagio</trd>
  <trd lng="pl">wolno<klr>(w muzyce)</klr></trd>
  <trd lng="sk">adagio <klr>(pomaly, v hudbe)</klr></trd>
</drv>
