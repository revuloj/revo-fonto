<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fragil.xml,v 1.12 2024/02/23 09:22:33 revo Exp $">
<kap>
  <rad>fragil</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="fragil.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="fragil.0a.KOMUNE">
    <dif>
      <refgrp tip="dif">
        <ref cel="fortik.mal0a.KOMUNE">Malfortika</ref>,
        <ref cel="romp.igxem0a.KOMUNE">rompi&gcirc;ema</ref>
      </refgrp>:
      <ekz>
        <tld/>a vivfadeno
        <fnt>
          <aut>W. Auld</aut>,
          <vrk><url ref="http://www.sunela.eu/maku/Esperanto/La%20infana%20raso">
          La Infana Raso</url></vrk>,
          <lok>1956</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi neniam supozis, ke en tiu &ccirc;i <tld/>a tenera junulino estas tiom da forto kaj
        volo
        <fnt>
          <aut>Julian Modest</aut>
            <vrk><url ref="&Monato;/2001/007140.html">Vetiha</url></vrk>
            <bib>Monato</bib>
            <lok>2001/02, p. 26</lok>
          </fnt>;
      </ekz>
      <ekz>
        malalta, <tld/>a knabo kun krispa hararo kaj brilaj okuloj kiel koloraj riveraj
        &scirc;tonetoj
        <fnt>
          <bib>Monato</bib>
          <aut>Julian Modest</aut>
          <vrk><url ref="&Monato;/2005/008708.php">Patrino</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sola transportrimedo konsistis el etaj <tld/>aj tradiciaj boatoj
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2007/009255.php">Insulo de verkistoj</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li estas poeto senpere lirika, kun tre <tld/>aj
        <klr>(<ref cel="delika.0a.io">delikata</ref>j)</klr> temoj
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        por fortikigi la <tld/>an pacon en Nord-Irlando estas enkondukitaj novaj
        iniciatoj
        <fnt>
       <aut>IRL</aut>
          <vrk><url ref="&Monato;/2000/006826.php">&Ccirc;u pa&scirc;oj al paco?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 7</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_o;&c_m;&c_k;&c_ib;</trd>,
    <trd>&c_k;&c_r;&c_o;&c_h;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="bo">&t_cha;&t_ga;&t_tsheg;&t_ba;&t_da;&t_drengbu;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>fragiln&iacute;</trd>,
    <trd>k&rcaron;ehk&yacute;</trd>,
    <trd>l&aacute;mav&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>fragil</trd>,
    <trd>zerbrechlich</trd>
  </trdgrp>
  <trd lng="fr">fr&ecirc;le</trd>
  <trd lng="nl">breekbaar</trd>
  <trdgrp lng="sk">
    <trd>krehk&yacute;</trd>,
    <trd>l&aacute;mav&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_je;&c_n;&c_d;&c_ib;&c_t;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_l;&c_a;&c_m;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#26131;&#25240;&#x65AD; <pr>y&#xEC;zh&#xE9;du&#xE0;n</pr></trd>,
    <trd>&#x4F1A;&#30772; <pr>hu&#xEC;p&#xF2;</pr></trd>,
    <trd>&#26131;&#x788E;&#x54C1; <pr>y&#xEC;su&#xEC;p&#x1D0;n</pr></trd>,
    <trd>&#x5A07;&#x8D35; <pr>ji&amacron;ogu&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>

