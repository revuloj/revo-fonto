<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: afisx.xml,v 1.30 2021/05/29 14:32:03 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>afi&scirc;</rad>/o
</kap>
<drv mrk="afisx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Sciigo al la publiko, presita sur papero kaj ordinare gluita sur
      muro.
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_f;&c_ib;&c_sh;&c_a;</trd>,
      <trd>&c_a;&c_b;'&c_ja;&c_v;&c_a;</trd>,
      <trd>&c_p;&c_l;&c_ja;&c_k;&c_a;&c_t;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Aushang</trd>,
      <trd>Plakat</trd>,
      <trd>Anschlag</trd>,
      <trd>Anschlagzettel</trd>
    </trdgrp>
    <trd lng="en">poster</trd>
    <trdgrp lng="es">
      <trd>cartel</trd>,
      <trd>p&oacute;ster</trd>,
      <trd>pancarta</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>affiche</trd>,
      <trd>avis<klr tip="ind"> (affiche)</klr></trd>,
      <trd>placard<klr tip="ind"> (affiche)</klr></trd>
    </trdgrp>
    <trd lng="it">manifesto</trd>
    <trdgrp lng="pl">
      <trd>afisz</trd>,
      <trd>plakat</trd>,
      <trd>og&lstroke;oszenie</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>cartaz</trd>,
      <trd>p&ocirc;ster</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>affiche</trd>,
      <trd>poster</trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>afi&scedil;</trd>,
      <trd>anun&tcedil;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_a;&c_f;&c_i;&c_sh;&c_a;</trd>,
      <trd>&c_o;&c_b;&c_malmol;&c_ja;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
    <trd lng="sk">plag&aacute;t</trd>
  </snc>
  <snc mrk="afisx.0o.komp">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      En komputilreta forumo a&ubreve; dissendolisto, mesa&gcirc;o
      sendenda al &ccirc;iuj ties abonantoj; <tld/>o estas speco de
      publika
      letero (artikolo), kontraste al privataj, persone adresitaj
      <ref cel="ret.0letero">retleteroj</ref>.
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_b;'&c_ja;&c_v;&c_a; <klr>(&c_p;&c_r;&c_a;&c_z;
      &c_Ib;&c_n;&c_t;&c_e;&c_r;&c_n;&c_e;&c_t;-&c_f;&c_o;&c_r;&c_u;&c_m;)</klr></trd>,
      <trd>&c_a;&c_r;&c_t;&c_y;&c_k;&c_u;&c_l; <klr>(&c_u;
      &c_t;&c_e;&c_l;&c_je;&c_k;&c_a;&c_n;&c_f;&c_e;&c_r;&c_e;&c_n;&c_c;&c_y;&c_ju;)</klr></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>post</trd>, <trd>poster</trd>,
      <trd>article</trd>
    </trdgrp>
    <trd lng="es">mensaje <klr>(en un foro de internet)</klr></trd>
    <trdgrp lng="fr">
      <trd>affiche<klr tip="ind">(dans un forum Internet)</klr></trd>
      <trd>article<klr tip="ind">(dans un forum Internet)</klr></trd>
      <trd>post<klr tip="ind">(dans un forum Internet)</klr></trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>pos</trd>,
      <trd>artikel</trd>,
      <trd>poster</trd>
    </trdgrp>
    <trd lng="it">post<klr> (comp.)</klr></trd>
    <trd lng="nl">bericht<klr tip="ind"> (in Internetforum)</klr></trd>
    <trd lng="pl">og&lstroke;oszenie</trd>
    <trd lng="pt"><ind>artigo</ind> (em f&oacute;rum
    eletr&ocirc;nico)</trd>
    <trd lng="ro">anun&tcedil;</trd>
    <trd lng="ru">&c_s;&c_t;&c_a;&c_t;&c_mol;&c_ja; <klr>(&c_v;
    &c_t;&c_je;&c_l;&c_je;&c_k;&c_o;&c_n;&c_f;&c_je;&c_r;&c_je;&c_n;&c_c;&c_i;&c_i;)</klr></trd>
  </snc>
  <trd lng="bo">&#xF66;&#xFA6;&#xFB1;&#xF62;&#xF0B;&#xF61;&#xF72;&#xF42;&#xF0B;</trd>
  <trdgrp lng="he">
    <trd>&#x5DE;&#x5D5;&#x5D3;&#x5E2;&#x5D4;</trd>,
    <trd>&#x5E4;&#x5D5;&#x5E1;&#x5D8;&#x5E8;</trd>
  </trdgrp>
  <trd lng="tr">afi&scedil;</trd>
</drv>

<drv mrk="afisx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Sciigi ion per <tld/>o:
      <ekz>
        li <tld/>is, ke morga&ubreve; li
        senpage razos.
      </ekz>
    </dif>
    <trd lng="be">&c_d;&c_a;&c_c;&c_mol;
    <ind>&c_a;&c_b;'&c_ja;&c_v;&c_u;</ind></trd>
    <trdgrp lng="fr">
      <trd>afficher</trd>,
      <trd>placarder</trd>
      <trd>apposer<klr tip="ind"> (un avis)</klr></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>aush&auml;ngen</trd>,
      <trd>plakatieren</trd>,
      <trd>anschlagen</trd>
    </trdgrp>
    <trd lng="es">pegar carteles</trd>
    <trdgrp lng="id">
      <trd>menge<ind>pos</ind></trd>,
      <trd>meng<ind>umum</ind>kan</trd>
    </trdgrp>
    <trd lng="it">mettere i manifesti<klr> (fig.)</klr></trd>
    <trd lng="nl">meedelen<klr tip="amb"> (op affiche)</klr></trd>
    <trdgrp lng="pl">
      <trd>da&cacute; <ind>og&lstroke;oszenie</ind></trd>,
      <trd>og&lstroke;asza&cacute;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>afixar</trd>,
      <trd>pregar cartazes</trd>
    </trdgrp>
    <trd lng="ru">&c_d;&c_a;&c_t;&c_mol;
    <ind>&c_o;&c_b;&c_malmol;&c_ja;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</ind></trd>
  </snc>
  <snc>
    <uzo tip="klr">(abs.)</uzo>
    <dif>
      Fiksi <tld/>on:
      <ekz>
        tie &ccirc;i malpermesate estas <tld/>i.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_l;&c_je;&c_ib;&c_c;&c_mol;
    <ind>&c_a;&c_f;&c_ib;&c_sh;&c_y;</ind></trd>
    <trdgrp lng="fr">
      <trd>afficher<klr tip="ind"> (abs.)</klr></trd>,
      <trd>apposer une <ind>affiche</ind></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>afficheren</trd>,
      <trd>aanplakken</trd>
    </trdgrp>
    <trd lng="pl">plakatowa&cacute;</trd>
    <trd lng="ru">&c_k;&c_l;&c_je;&c_i;&c_t;&c_mol; 
    <ind>&c_a;&c_f;&c_i;&c_sh;&c_i;</ind></trd>
  </snc>
  <snc>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Sendi <tld/>on en komputilretan forumon.
    </dif>
    <trd lng="be">&c_d;&c_a;&c_s;&c_l;&c_a;&c_c;&c_mol;
    <ind>&c_a;&c_r;&c_t;&c_y;&c_k;&c_u;&c_l;</ind> &c_u;
    &c_t;&c_e;&c_l;&c_je;&c_k;&c_a;&c_n;&c_f;&c_e;&c_r;&c_e;&c_n;&c_c;&c_y;&c_ju;</trd>
    <trd lng="es">adjuntar fichero, cartel<klr tip="amb"> (en un mensaje
    de ordenador)</klr></trd>
    <trdgrp lng="fr">
      <trd>afficher<klr tip="ind">(dans un forum Internet)</klr></trd>,
      <trd>poster<klr tip="ind">(dans un forum Internet)</klr></trd>
    </trdgrp>
    <trd lng="nl">posten <klr tip="ind">(een
    Internetbericht)</klr></trd>
    <trd lng="pt"><ind>enviar artigo</ind> (a um f&oacute;rum
    eletr&ocirc;nico)</trd>
    <trd lng="ru">&c_p;&c_o;&c_s;&c_l;&c_a;&c_t;&c_mol;
    <ind>&c_s;&c_t;&c_a;&c_t;&c_mol;&c_ju;</ind> &c_v;
    &c_t;&c_je;&c_l;&c_je;&c_k;&c_o;&c_n;&c_f;&c_je;&c_r;&c_je;&c_n;&c_c;&c_i;&c_ju;</trd>
  </snc>
    <trd lng="en">to <ind>post</ind></trd>
    <trdgrp lng="he">
    <trd>&#x5DC;&#x5D4;&#x5D5;&#x5D3;&#x5D9;&#x5E2;</trd>,
    <trd>&#x5DC;&#x5E4;&#x5E8;&#x5E1;&#x5DD;</trd>
  </trdgrp>
  <trd lng="it">affiggere</trd>
  <trd lng="tr">yollamak</trd>
</drv>
</art>
