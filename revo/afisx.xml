<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>afi&scirc;</rad>/o
</kap>
<drv mrk="afisx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Sciigo al la publiko, presita sur papero kaj ordinare gluita sur
      muro.
    </dif>
    <trdgrp lng="de">
      <trd>Aushang</trd>,
      <trd>Plakat</trd>,
      <trd>Anschlag</trd>,
      <trd>Anschlagzettel</trd>
    </trdgrp>
    <trd lng="en">poster</trd>
    <trdgrp lng="es">
      <trd>cartel</trd>,
      <trd>poster</trd>,
      <trd>pancarta</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>affiche</trd>,
      <trd>avis</trd>,
      <trd>placard</trd>
    </trdgrp>
    <trd lng="it">manifesto</trd>
    <trdgrp lng="pt">
      <trd>cartaz</trd>,
      <trd>p&ocirc;ster</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>affiche</trd>,
      <trd>poster</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_a;&c_f;&c_i;&c_sh;&c_a;</trd>,
      <trd>&c_o;&c_b;&c_malmol;&c_ja;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc mrk="afisx.0o.komp">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      En komputilreta forumo a&ubreve; dissendolisto, mesa&gcirc;o
      sendenda al &ccirc;iuj ties abonantoj; <tld/>o estas speco de publika
      letero (artikolo), kontraste al privataj, persone adresitaj
      <ref cel="ret.0letero">retleteroj</ref>.
    </dif>
    <trdgrp lng="en">
      <trd>post</trd>, <trd>poster</trd>,
      <trd>article</trd>
    </trdgrp>
    <trd lng="es">mensaje <klr>(en un foro de internet)</klr></trd>
    <trdgrp lng="fr">
      <trd>affiche</trd>,
      <trd>article <klr>(dans un forum Internet)</klr></trd>
    </trdgrp>
    <trd lng="nl">bericht<klr tip="ind"> (in Internetforum)</klr></trd>
    <trd lng="pt"><ind>artigo</ind> (em f&oacute;rum eletr&ocirc;nico)</trd>
    <trd lng="ru">&c_s;&c_t;&c_a;&c_t;&c_mol;&c_ja; <klr>(&c_v; &c_t;&c_je;&c_l;&c_je;&c_k;&c_o;&c_n;&c_f;&c_je;&c_r;&c_je;&c_n;&c_c;&c_i;&c_i;)</klr></trd>
  </snc>
</drv>

<drv mrk="afisx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Sciigi ion per <tld/>o:
      <ekz>
        li <tld/>is, ke morga&ubreve; li
        senpage razos.
      </ekz>
    </dif>
    <trd lng="fr">afficher</trd>
    <trdgrp lng="de">
      <trd>aush&auml;ngen</trd>,
      <trd>plakatieren</trd>,
      <trd>anschlagen</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>afficheren</trd>,
      <trd>aanplakken</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>afixar</trd>,
      <trd>pregar cartazes</trd>
    </trdgrp>
    <trd lng="ru">&c_d;&c_a;&c_t;&c_mol; <ind>&c_o;&c_b;&c_malmol;&c_ja;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</ind></trd>
  </snc>
  <snc>
    <uzo tip="klr">(abs.)</uzo>
    <dif>
      Fiksi <tld/>on:
      <ekz>
        tie &ccirc;i malpermesate estas <tld/>i.
      </ekz>
    </dif>
    <trd lng="fr">afficher</trd>
    <trd lng="ru">&c_k;&c_l;&c_je;&c_i;&c_t;&c_mol;  <ind>&c_a;&c_f;&c_i;&c_sh;&c_i;</ind></trd>
  </snc>
  <snc>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Sendi <tld/>on en komputilretan forumon.
    </dif>
    <trd lng="en">to <ind>post</ind></trd>
    <trdgrp lng="fr">
      <trd>afficher</trd>,
      <trd>poster<klr> (un article, une affiche)</klr></trd>
    </trdgrp>
    <trd lng="nl">posten <klr tip="ind">(een Internetbericht)</klr></trd>
    <trd lng="pt"><ind>enviar artigo</ind> (a um f&oacute;rum eletr&ocirc;nico)</trd>
    <trd lng="ru">&c_p;&c_o;&c_s;&c_l;&c_a;&c_t;&c_mol; <ind>&c_s;&c_t;&c_a;&c_t;&c_mol;&c_ju;</ind> &c_v;
    &c_t;&c_je;&c_l;&c_je;&c_k;&c_o;&c_n;&c_f;&c_je;&c_r;&c_je;&c_n;&c_c;&c_i;&c_ju;</trd>
  </snc>
</drv>
</art>
