<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: afisx.xml,v 1.38 2024/04/04 06:26:03 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>afi&scirc;</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="afisx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="sci.0igo">Sciigo</ref> al la publiko, presita sur papero kaj ordinare gluita sur
      muro a&ubreve; portebla tabulo:
      <ekz>
        mi vidis sur la <tld/>o la Belan Helenon kaj tamen ne iris en la teatron
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i citis de la <tld/>oj, &leftquot;plej fama svedlanda &jcirc;azkapelo el Stokholmo&rightquot;
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi vidas la <tld/>on sur la muro: &#8218;Eniro Malpermesata&#8216;
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 2</lok>
        </fnt>;
      </ekz>
      <ekz>
        super amasi&gcirc;inta aliborde homgrupo balanci&gcirc;as diverskolora <tld/>o kun portreto
        de <nom>Gindenburg</nom>
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi haltis apud reklama koloneto kaj &scirc;ajnigas, ke mi legas la <tld/>on
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la manoj li tenis grandegan <tld/>on, alvokantan vo&ccirc;doni kontra&ubreve; konstruo de
        la kiras&scirc;ipo
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tabul.anonc0o">anonctabulo</ref>,
      <ref cel="bander.0o.devizo">banderolo</ref>,
      <ref cel="reklam.0ilo">reklamilo</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_a;&c_f;&c_ib;&c_sh;&c_a;</trd>,
      <trd>&c_a;&c_b;'&c_ja;&c_v;&c_a;</trd>,
      <trd>&c_p;&c_l;&c_ja;&c_k;&c_a;&c_t;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Aushang</trd>,
      <trd>Plakat</trd>,
      <trd>Anschlag <klr>(Zettel)</klr></trd>,
      <trd>Anschlagzettel</trd>
    </trdgrp>
    <trd lng="en">poster</trd>
    <trdgrp lng="es">
      <trd>cartel</trd>,
      <trd>p&oacute;ster</trd>,
      <trd>pancarta</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>affiche</trd>,
      <trd>avis<klr tip="ind"> (affiche)</klr></trd>,
      <trd>placard<klr tip="ind"> (affiche)</klr></trd>
    </trdgrp>
    <trd lng="it">manifesto</trd>
    <trdgrp lng="pl">
      <trd>afisz</trd>,
      <trd>plakat</trd>,
      <trd>og&lstroke;oszenie</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>cartaz</trd>,
      <trd>p&ocirc;ster</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>affiche</trd>,
      <trd>poster</trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>afi&scomma;</trd>,
      <trd>anun&tcomma;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_a;&c_f;&c_i;&c_sh;&c_a;</trd>,
      <trd>&c_o;&c_b;&c_malmol;&c_ja;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
    <trd lng="sk">plag&aacute;t</trd>
  </snc>
  <snc mrk="afisx.0o.komp">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      En komputilreta <ref tip="malprt" cel="forum.0o.komp">forumo</ref>
      a&ubreve; <ref tip="malprt" cel="list.dissendo0o">dissendolisto</ref>, <ref tip="super" cel="mesagx.0o">mesa&gcirc;o</ref>
      sendenda al &ccirc;iuj ties abonantoj;
      <ekz>
         <tld/>o estas speco de
         publika letero (artikolo), kontraste al privataj, persone adresitaj
         <ref cel="leter.ret0o">retleteroj</ref>;
      </ekz>
      <ekz>
        &gcirc;i i&gcirc;is malaktiva organizo, ilia lasta <nom>Facebook</nom>-<tld/>o estas de anta&ubreve; pli ol 5
        jaroj
        <fnt>
          <bib>Kon11</bib>
          <lok>TEJO fari&gcirc;is membro de UNOY-Peacebuilders</lok>
        </fnt>;
      </ekz>
      <ekz>
        disdono de &scirc;iaj personaj spertoj <klr>[&#8230;]</klr> tra blog<tld/>oj, retartikoloj
        <fnt>
          <bib>Kon11</bib>
          <lok>Plurfaza Dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Spillner</nom> prezentas al ni socimedian retejon, kiu kompense al popularaj <tld/>oj,
        pagas la a&ubreve;torojn per alia cifereca mono
        <fnt>
          <bib>Kon11</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_b;'&c_ja;&c_v;&c_a; <klr>(&c_p;&c_r;&c_a;&c_z;
      &c_Ib;&c_n;&c_t;&c_e;&c_r;&c_n;&c_e;&c_t;-&c_f;&c_o;&c_r;&c_u;&c_m;)</klr></trd>,
      <trd>&c_a;&c_r;&c_t;&c_y;&c_k;&c_u;&c_l; <klr>(&c_u;
      &c_t;&c_e;&c_l;&c_je;&c_k;&c_a;&c_n;&c_f;&c_e;&c_r;&c_e;&c_n;&c_c;&c_y;&c_ju;)</klr></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Beitrag <klr>(Forum)</klr></trd>,
      <trd>Post <klr>(Internet)</klr></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>post</trd>, <trd>poster</trd>,
      <trd>article</trd>
    </trdgrp>
    <trd lng="es">mensaje <klr>(en un foro de internet)</klr></trd>
    <trdgrp lng="fr">
      <trd>affiche<klr tip="ind">(dans un forum Internet)</klr></trd>
      <trd>article<klr tip="ind">(dans un forum Internet)</klr></trd>
      <trd>post<klr tip="ind">(dans un forum Internet)</klr></trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>pos</trd>,
      <trd>artikel</trd>,
      <trd>poster</trd>
    </trdgrp>
    <trd lng="it">post<klr> (comp.)</klr></trd>
    <trd lng="nl">bericht<klr tip="ind"> (in Internetforum)</klr></trd>
    <trd lng="pl">og&lstroke;oszenie</trd>
    <trd lng="pt"><ind>artigo</ind> (em f&oacute;rum
    eletr&ocirc;nico)</trd>
    <trd lng="ro">anun&tcomma;</trd>
    <trd lng="ru">&c_s;&c_t;&c_a;&c_t;&c_mol;&c_ja; <klr>(&c_v;
    &c_t;&c_je;&c_l;&c_je;&c_k;&c_o;&c_n;&c_f;&c_je;&c_r;&c_je;&c_n;&c_c;&c_i;&c_i;)</klr></trd>
    <trd lng="zh">&#20844;&#21578; <pr>g&omacron;ngg&agrave;o</pr></trd>
</snc>
  <trd lng="bo">&t_sa;&t_bata;&t_yata;&t_ra;&t_tsheg;&t_ya;&t_gigu;&t_ga;&t_tsheg;</trd>
  <trdgrp lng="he">
    <trd>&mem;&vav;&dalet;&ayin;&he;</trd>,
    <trd>&pe;&vav;&samekh;&tet;&resh;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12509;&#12473;&#12479;&#12540;  <pr>&#12413;&#12377;&#12383;&#12540;</pr></trd>,
    <trd>&#36028;&#12426;&#32025;  <pr>&#12399;&#12426;&#12364;&#12415;</pr></trd>,
    <trd>&#25522;&#31034;&#29289;  <pr>&#12369;&#12356;&#12376;&#12406;&#12388;</pr></trd>,
    <trd>&#12503;&#12521;&#12459;&#12540;&#12489;</trd>,
    <trd>&#25237;&#31295;&#35352;&#20107;  <pr>&#12392;&#12358;&#12371;&#12358;&#12365;&#12376;</pr></trd>,
    <trd>&#25522;&#31034;  <pr>&#12369;&#12356;&#12376;</pr></trd>
  </trdgrp>
  <trd lng="tr">afi&scedil;</trd>
  <trdgrp lng="uk">
    <trd>&c_a;&c_f;&c_ib;&c_sh;&c_a;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_k;&c_a;&c_t;</trd>,
    <trd>&c_o;&c_g;&c_o;&c_l;&c_o;&c_sh;&c_je;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="afisx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="afisx.0i.sciigi">
    <dif>
      <ref tip="super" cel="sci.0igi">Sciigi</ref> ion per <tld/>o:
      <ekz>
        li <tld/>is, ke morga&ubreve; li senpage razos;
      </ekz>
      <ekz>
        mia banko afable <tld/>as &ccirc;e la elpaga&ubreve;tomato, ke &leftquot;pro via sekureco&rightquot; oni filmas
        min
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2008/009525.php">Fratego fiaskas</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_d;&c_a;&c_c;&c_mol;
    <ind>&c_a;&c_b;'&c_ja;&c_v;&c_u;</ind></trd>
    <trdgrp lng="fr">
      <trd>afficher</trd>,
      <trd>placarder</trd>
      <trd>apposer<klr tip="ind"> (un avis)</klr></trd>
    </trdgrp>
    <trd lng="de">aush&auml;ngen</trd>
    <trd lng="es">pegar carteles</trd>
    <trdgrp lng="id">
      <trd>menge<ind>pos</ind></trd>,
      <trd>meng<ind>umum</ind>kan</trd>
    </trdgrp>
    <trd lng="it">mettere i manifesti<klr> (fig.)</klr></trd>
    <trd lng="nl">meedelen<klr tip="amb"> (op affiche)</klr></trd>
    <trdgrp lng="pl">
      <trd>da&cacute; <ind>og&lstroke;oszenie</ind></trd>,
      <trd>og&lstroke;asza&cacute;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>afixar</trd>,
      <trd>pregar cartazes</trd>
    </trdgrp>
    <trd lng="ru">&c_d;&c_a;&c_t;&c_mol;
    <ind>&c_o;&c_b;&c_malmol;&c_ja;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</ind></trd>
  </snc>
  <snc mrk="afisx.0i.fiksi">
    <uzo tip="klr">(abs.)</uzo>
    <dif>
      <ref tip="super" cel="fiks.0i.flagon">Fiksi</ref> <tld/>on:
      <ekz>
        ni traveturis la urbon, ser&ccirc;ante lokojn por <tld/>i
        <fnt>
          <bib>KrM</bib>
          <lok>40. Kaj Rapide Kreskas La Afero.</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi disdonis flugfoliojn kaj <tld/>is sur muroj proksime de la konferencejo
        <fnt>
          <bib>Kon11</bib>
          <lok>Esperanto vivas anka&ubreve; en la araba Nord-Afriko</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_l;&c_je;&c_ib;&c_c;&c_mol;
    <ind>&c_a;&c_f;&c_ib;&c_sh;&c_y;</ind></trd>
    <trdgrp lng="de">
      <trd>plakatieren</trd>,
      <trd>anschlagen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>afficher<klr tip="ind"> (abs.)</klr></trd>,
      <trd>apposer une <ind>affiche</ind></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>afficheren</trd>,
      <trd>aanplakken</trd>
    </trdgrp>
    <trd lng="pl">plakatowa&cacute;</trd>
    <trd lng="ru">&c_k;&c_l;&c_je;&c_i;&c_t;&c_mol;
    <ind>&c_a;&c_f;&c_i;&c_sh;&c_i;</ind></trd>
  </snc>
  <snc mrk="afisx.0i.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <ref tip="super" cel="send.0i.ion">Sendi</ref> <tld/>on en komputilretan forumon:
      <ekz>
        <tld/>i siajn komentojn en retforumo
        <fnt>
          <bib>Kon11</bib>
          <lok>Intervjuo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mastrumi retejojn kaj alloge <tld/>i materialojn, ene de komforte uzebla
        strukturo
        <fnt>
          <bib>Kon11</bib>
          <lok>Alvoko &ndash; vere ur&gcirc;as trovi helpantojn!</lok>
        </fnt>;
      </ekz>
      <ekz>
        a&ubreve;stralia esperantisto praktike &ccirc;iutage <tld/>as novan videobloga&jcirc;on
        <fnt>
          <bib>Kon11</bib>
          <lok>Intervjuo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_d;&c_a;&c_s;&c_l;&c_a;&c_c;&c_mol;
    <ind>&c_a;&c_r;&c_t;&c_y;&c_k;&c_u;&c_l;</ind> &c_u;
    &c_t;&c_e;&c_l;&c_je;&c_k;&c_a;&c_n;&c_f;&c_e;&c_r;&c_e;&c_n;&c_c;&c_y;&c_ju;</trd>
    <trdgrp lng="de">
      <trd>posten <klr>(Beitrag)</klr></trd>,
      <trd>teilen <klr>(Beitrag)</klr></trd>,
      <trd>sharen <klr>(Beitrag)</klr></trd>
    </trdgrp>
    <trd lng="en">to <ind>share</ind></trd>
    <trd lng="es">adjuntar fichero, cartel<klr tip="amb"> (en un mensaje
    de ordenador)</klr></trd>
    <trdgrp lng="fr">
      <trd>afficher<klr tip="ind">(dans un forum Internet)</klr></trd>,
      <trd>poster<klr tip="ind">(dans un forum Internet)</klr></trd>
    </trdgrp>
    <trd lng="nl">posten <klr tip="ind">(een
    Internetbericht)</klr></trd>
    <trd lng="pt"><ind>enviar artigo</ind> (a um f&oacute;rum
    eletr&ocirc;nico)</trd>
    <trd lng="ru">&c_p;&c_o;&c_s;&c_l;&c_a;&c_t;&c_mol;
    <ind>&c_s;&c_t;&c_a;&c_t;&c_mol;&c_ju;</ind> &c_v;
    &c_t;&c_je;&c_l;&c_je;&c_k;&c_o;&c_n;&c_f;&c_je;&c_r;&c_je;&c_n;&c_c;&c_i;&c_ju;</trd>
  </snc>
  <refgrp tip="vid">
    <ref cel="publik.0igi">publikigi</ref>
  </refgrp>
  <trd lng="en">to <ind>post</ind></trd>
  <trdgrp lng="he">
    <trd>&lamed;&he;&vav;&dalet;&yod;&ayin;</trd>,
    <trd>&lamed;&pe;&resh;&samekh;&fmem;</trd>
  </trdgrp>
  <trd lng="it">affiggere</trd>
  <trdgrp lng="ja">
    <trd>&#25522;&#31034;&#12377;&#12427;  <pr>&#12369;&#12356;&#12376;&#12377;&#12427;</pr></trd>,
    <trd>&#25237;&#31295;&#12377;&#12427;  <pr>&#12392;&#12358;&#12371;&#12358;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="tr">yollamak</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_z;&c_k;&c_l;&c_je;&c_ju;&c_v;&c_a;&c_t;&c_i; (&c_a;&c_f;&c_ib;&c_sh;&c_ib;</trd>,
    <trd>&c_o;&c_g;&c_o;&c_l;&c_o;&c_sh;&c_je;&c_n;&c_n;&c_ja;)</trd>,
    <trd>&c_v;&c_i;&c_v;&c_ib;&c_sh;&c_u;&c_v;&c_a;&c_t;&c_i; (&c_p;&c_l;&c_a;&c_k;&c_a;&c_t;&c_i;)</trd>
  </trdgrp>
</drv>

<drv mrk="afisx.art0o">
  <kap>
    art<tld/>o
  </kap>
  <snc>
    <dif>
      <ref cel="grafik.0ajxo">Grafika&jcirc;o</ref>, arta presa&jcirc;o, en formo de afi&scirc;ebla granda folio:
      <ekz>
        art<tld/>o de <nom>Rafa&lstroke; Olbi&nacute;ski</nom> por la opero &leftquot;Re&gcirc;o <nom>Roger</nom>&rightquot;
        <fnt>
          <aut>Barbara Pietrzak</aut>
          <vrk><url ref="https://pola-retradio.org/2019/10/e_elsendo-el-la-04-10-2019/"
              >La 899-a E_elsendo</url></vrk>
          <lok>Pola Retradio, elsendo 2019-10-04</lok>
        </fnt>;
      </ekz>
      <ekz>
        historio de unu art<tld/>o
        <fnt>
          <vrk><url ref="https://www.esperanto.be/fel/nl/Archief/panorama045.pdf"
              >Nieuwe brochure van LOT</url></vrk>
          <lok>Esperanto Panorama, n-ro 3, 5-6/1977, p. 4</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Poster</trd>
  <trd lng="en">poster</trd>
  <trd lng="fr">poster</trd>
</drv>

</art>
