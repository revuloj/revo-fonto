<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>altitud</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="altitud.0o">
  <kap><tld/>o</kap>
  <snc mrk="altitud.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="vid" cel="alt.0eco.eco">Alteco</ref> super la marnivelo:
      <ekz>
        kolizio inter aviadiloj Tupolev kaj Boeing je <tld/>o de 12 000
        m
        <fnt>
        <aut>L. G. Restrepo Rivas, H. Masson</aut>,
        <vrk>&Ccirc;u aldona&jcirc;o por la &leftquot;Socia kosto de la Angla&rightquot;?</vrk>,
        <lok>soc.culture.esperanto, 2002-07-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">altitud</trd>
  <trd lng="de">H&ouml;he<klr> (&uuml;ber dem Meeresspiegel)</klr></trd>
  <trd lng="fr">altitude</trd>
  <trd lng="hu">tengerszint feletti <ind>magass&aacute;g</ind></trd>
  <trd lng="pt">altitude</trd>
  <trd lng="ru">&c_v;&c_y;&c_s;&c_o;&c_t;&c_a; &c_n;&c_a;&c_d; &c_u;&c_r;&c_o;&c_v;&c_n;&c_je;&c_m; &c_m;&c_o;&c_r;&c_ja;</trd>
</drv>
</art>

