<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>natr</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="natr.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      CO<sub>3</sub>Na<sub>2</sub>. Senkolora alkala salo, natria
      karbonato, facile solvebla en akvo kaj tre uzata en industrio
      (fabrikado de vitro, sapo ktp.).
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_w;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_ja; &c_s;&c_o;&c_d;&c_a;</trd>,
    <trd>&c_je;&c_d;&c_k;&c_ib; &c_n;&c_a;&c_t;&c_r;</trd>
  </trdgrp>
  <trd lng="de">Natron</trd>
  <trdgrp lng="en">
    <trd>caustic <ind>soda</ind></trd>,
    <trd>lye</trd>
  </trdgrp>
  <trd lng="hu">sz&oacute;da</trd>
  <trd lng="nl">loogzout</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_u;&c_s;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja; &c_s;&c_o;&c_d;&c_a;</trd>,
    <trd>&c_je;&c_d;&c_k;&c_i;&c_j; <ind>&c_n;&c_a;&c_t;&c_r;</ind></trd>
  </trdgrp>
</drv>
</art>
