<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: suom.xml,v 1.13 2024/03/25 07:28:37 revo Exp $">
<kap>
  <rad>suom</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>

<drv mrk="suom.0o">
  <kap><tld/>o</kap>
  <snc mrk="suom.0o.POL">
    <dif>
     <ref tip="dif" cel="finn.0o">Finno</ref>:
      <ekz>
        en <nom>Tampere</nom> mi renkontis homojn &mdash; ne <tld/>ojn, hungarojn,
        japanojn kaj brazilanojn, sed homojn
        <fnt>
          <vrk><url ref="http://www.esperanto.org/us/USEJ/ktp/1995/sep-okt/guy-ijk.html">Guy
          Menahem's trip to the '95 IJK</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        italo lernas kun japano kaj <tld/>ino helpe de afrika
        instruisto
        <fnt>
          <vrk>Ni demandis al Ma&ubreve;ro La Torre&#x2026;</vrk>.
          <lok>Internacia Pedagogia Revuo, 2006:4, p. 11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_n;</trd>
  <trd lng="bg">&c_f;&c_i;&c_n;&c_l;&c_a;&c_n;&c_d;&c_je;&c_c;</trd>
  <trd lng="cs">Fin</trd>
  <trd lng="de">Finne</trd>
  <trd lng="en">Finn</trd>
  <trd lng="fa">&a_f;&a_n;&a_l;&a_A;&a_n;&a_d;&f_y;</trd>
  <trd lng="fr">Finlandais<klr> (habitant)</klr></trd>
  <trdgrp lng="no">
    <trd>finne</trd>,
    <trd>finlender</trd>,
    <trd>finlending</trd>
  </trdgrp>
  <trd lng="nl">Fin</trd>
  <trd lng="pl">Fin</trd>
  <trd lng="ru">&c_f;&c_i;&c_n;&c_n;</trd>
  <trd lng="sk">F&iacute;n</trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x443;&#x43C;</trd>,
    <trd>&#x441;&#x443;&#x43E;&#x43C;&#x456;</trd>,
    <trd>&#x444;&#x456;&#x43D;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x82AC;&#x5170;&#x4EBA; <pr>f&#x113;nl&#xE1;nr&#xE9;n</pr></trd>,
    <trd>&#x82AC;&#x862D;&#x4EBA; <pr>f&#x113;nl&#xE1;nr&#xE9;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="suom.0a">
  <kap><tld/>a</kap>
  <snc mrk="suom.0a.finna">
    <dif>
      <ref tip="dif" cel="finn.0a">Finna</ref>:
      <ekz>
        la&ubreve; recenzo de juna <tld/>a Esperantisto <nom>Tapani Kuivala</nom>, 
        <klr>[la libro estas]</klr> <ctl>teda, plena de vana babilado kaj manka je metiarto</ctl>
        <fnt>
          <aut>Jorge Camacho (Georgo Kama&ccirc;o)</aut>:
          <vrk><url ref="http://esperanto.net/literaturo/roman/libr/romaneseo.html">Originalaj romanoj en Esperanto: panoramo kaj
              perspektivoj</url></vrk> 
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_n;&c_s;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_f;&c_i;&c_n;&c_s;&c_k;&c_i;</trd>
  <trd lng="de">finnisch</trd>
  <trd lng="en">Finnish</trd>
  <trd lng="fa">&a_f;&a_n;&a_l;&a_A;&a_n;&a_d;&f_y;</trd>
  <trd lng="fr">finlandais<klr> (adj.)</klr></trd>
  <trd lng="nl">Fins</trd>
  <trd lng="no">finsk</trd>
  <trd lng="pl">fi&nacute;ski</trd>
  <trd lng="ru">&c_f;&c_i;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x82AC;&#x5170;&#x8BED; <pr>F&#x113;nl&#xE1;ny&#x1D4;</pr></trd>,
    <trd>&#x82AC;&#x862D;&#x8A9E; <pr>F&#x113;nl&#xE1;ny&#x1D4;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="suom.S0ujo">
  <kap><tld lit="S"/>ujo</kap>
  <snc mrk="suom.S0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="finn.F0lando">Finnlando</ref>:
      <ekz>
        la &scirc;tato havis la mandaton regi la naciajn poseda&jcirc;ojn de
        <tld lit="S"/>ujo, kaj tiel i&gcirc;is socipolitika ilo
        <fnt>
          <aut>Laura Kolbe, tr. Marko Rauhamaa</aut>:
          <vrk><url ref="ftp://ftp.stack.nl/pub/esperanto/esperanto-texts.dir/kolbex/kolbe-cxefo.html">Hejmo,
          familio kaj bonfart&scirc;tato &mdash; e&ubreve;ropaj realigoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_F;&c_ib;&c_n;&c_l;&c_ja;&c_n;&c_d;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_F;&c_i;&c_n;&c_l;&c_a;&c_n;&c_d;&c_i;&c_ja;</trd>
  <trd lng="ca">Finl&agrave;ndia</trd>
  <trd lng="cs">Finsko</trd>
  <trd lng="de">Finnland</trd>
  <trd lng="en">Finnland</trd>
  <trd lng="es">Finlandia</trd>
  <trd lng="eu">Finlandia</trd>
  <trd lng="fa">&a_f;&a_n;&a_l;&a_A;&a_n;&a_d;</trd>
  <trdgrp lng="fr">
    <trd>Finlande</trd>,
    <trd>R&eacute;publique de Finlande</trd>
  </trdgrp>
  <trd lng="fy">Finl&acirc;n</trd>
  <trd lng="he">&pe;&yod;&nun;&lamed;&nun;&dalet;</trd>
  <trd lng="io">Finlando</trd>
  <trd lng="nl">Finland</trd>
  <trd lng="no">Finland</trd>
  <trd lng="pl">Finlandia</trd>
  <trd lng="pt">Finl&acirc;ndia</trd>
  <trd lng="ru">&c_F;&c_i;&c_n;&c_l;&c_ja;&c_n;&c_d;&c_i;&c_ja;</trd>
  <trd lng="sk">F&iacute;nsko</trd>
  <trd lng="tok">ma Sumi</trd>
  <trd lng="tr">Finlandiya</trd>
  <trd lng="vo">Suomiy&auml;n</trd>
  <trdgrp lng="zh">
    <trd>&#x82AC;&#x5170; <pr>F&#x113;nl&#xE1;n</pr></trd>,
    <trd>&#x82AC;&#x862D; <pr>F&#x113;nl&#xE1;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="suom.0alingvo">
  <kap><tld/>a lingvo</kap>
  <snc mrk="suom.0alingvo.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="dif" cel="finn.0alingvo">Finna lingvo</ref>:
      <ekz>
        la finna lingvo, <tld/>a lingvo <klr>[&#x2026;]</klr> estas aglutina lingvo el la
        finn-ugra lingvaro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1186">Finna lingvo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_n;&c_s;&c_k;&c_a;&c_ja; &c_m;&c_o;&c_v;&c_a;</trd>
  <trd lng="en">Finnish</trd>
  <trd lng="fr">finlandaise<klr> (langue)</klr></trd>
  <trd lng="fy">Finsk</trd>
  <trd lng="io">Finlandana linguo</trd>
  <trd lng="nl">Fins</trd>
  <trd lng="ru">&c_f;&c_i;&c_n;&c_s;&c_k;&c_i;&c_j; &c_ja;&c_z;&c_y;&c_k;</trd>
  <trd lng="tok">toki Sumi</trd>
  <trd lng="vo">Suomiy&auml;nap&uuml;k</trd>
  <trdgrp lng="zh">
    <trd>&#x82AC;&#x5170;&#x8BED; <pr>F&#x113;nl&#xE1;ny&#x1D4;</pr></trd>,
    <trd>&#x82AC;&#x862D;&#x8A9E; <pr>F&#x113;nl&#xE1;ny&#x1D4;</pr></trd>
  </trdgrp>
</drv>

