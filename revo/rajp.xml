<vortaro>
  <art mrk="$Id: rajp.xml,v 1.2 2020/06/08 09:10:32 afido Exp $">
    <kap>
      <rad>rajp</rad>/i
    </kap>

<drv mrk="rajp.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">TEK</uzo><uzo tip="fak">FIZ</uzo>
  <snc mrk="rajp.0i.TEK">
    <dif>
      Froti&gcirc;i, <ref tip="dif" cel="frot.dis0i">disfroteti</ref>:
      <ekz>
        akso rajpis kaj la orienti&gcirc;o de la instrumentoj kunportitaj de
        la sondilo i&gcirc;is
        preska&ubreve; nekontrolebla<klr>[j]</klr>
        <fnt>
          <vrk><url
          ref="https://www.bertin.biz/index.php?titre=voyager2.htm">Kun
          &leftquot;Voyager 2&rightquot; proksime de Neptuno</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        post dek minutoj <tld/>is lia rapidum&scirc;an&gcirc;ilo, damne!
        <fnt>
          <aut>Jen&odblac; Rejt&odblac;</aut>
          <vrk><url
          ref="https://www.you-books.com/book/J-Rejto/La-dekkvar-karata-automobilo">La
          dekkvar-karata a&ubreve;tomobilo</url></vrk>
        </fnt>
      </ekz>
    </dif>
    <trd lng="de">reiben (Mechanik)</trd>
    <trdgrp lng="en">
      <trd>rub</trd>,
      <trd>chafe</trd>
    </trdgrp>
      <trd lng="ru">&c_z;&c_a;&c_je;&c_d;&c_a;&c_t;&c_mol; (&c_t;&c_je;&c_h;&c_n;.)</trd></snc>
</drv>

  </art>

