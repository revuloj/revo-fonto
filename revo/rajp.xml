  <art mrk="$Id: rajp.xml,v 1.8 2024/04/13 20:15:51 afido Exp $">
    <kap>
      <rad>rajp</rad>/i
    </kap>

<drv mrk="rajp.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">TEK</uzo><uzo tip="fak">FIZ</uzo>
  <snc mrk="rajp.0i.TEK">
    <dif>
      Froti&gcirc;i, <ref tip="dif" cel="frot.dis0i">disfroteti</ref>:
      <ekz>
        akso rajpis kaj la orienti&gcirc;o de la instrumentoj kunportitaj de
        la sondilo i&gcirc;is
        preska&ubreve; nekontrolebla<klr>[j]</klr>
        <fnt>
          <vrk><url ref="https://www.bertin.biz/index.php?titre=voyager2.htm">Kun
          &leftquot;Voyager 2&rightquot; proksime de Neptuno</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        post dek minutoj <tld/>is lia rapidum&scirc;an&gcirc;ilo, damne!
        <fnt>
          <aut>Jen&odblac; Rejt&odblac;</aut>
          <vrk><url ref="https://www.you-books.com/book/J-Rejto/La-dekkvar-karata-automobilo">La
          dekkvar-karata a&ubreve;tomobilo</url></vrk>
        </fnt>
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_a;&c_ja;&c_d;&c_a;&c_c;&c_mol; <klr>(&c_p;&c_r;&c_a;&c_c;&c_a;&c_v;&c_a;&c_c;&c_mol; &c_z; &c_z;&c_a;&c_m;&c_je;&c_sh;&c_k;&c_a;&c_m;&c_ib;)</klr></trd>
    <trd lng="de">reiben (Mechanik)</trd>
    <trdgrp lng="en">
      <trd>rub</trd>,
      <trd>chafe</trd>
    </trdgrp>
    <trd lng="pl">zaciera&cacute; si&eogonek; </trd>
    <trd lng="ro">s&abreve; se pr&abreve;bu&#x219;easc&abreve;</trd>
    <trd lng="ru">&c_z;&c_a;&c_je;&c_d;&c_a;&c_t;&c_mol; (&c_t;&c_je;&c_h;&c_n;.)</trd>  <trdgrp lng="zh">
    <trd>&#x6469;&#x64E6; <pr>m&#xF3;c&#x101;</pr></trd>,
    <trd>&#x633C; <pr>ru&#xF3;</pr></trd>
  </trdgrp>
</snc>
    <trdgrp lng="ja">
    <trd>&#x713C;&#x304D;&#x3064;&#x304F;  <pr>&#x3084;&#x304D;&#x3064;&#x304F;</pr></trd>,
    <trd>&#x52D5;&#x304B;&#x306A;&#x304F;&#x306A;&#x308B;  <pr>&#x3046;&#x3054;&#x304B;&#x306A;&#x304F;&#x306A;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="uk">&c_z;&c_a;&c_ji;&c_d;&c_a;&c_t;&c_i;</trd>
</drv>

