<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Albert</rad>/o
</kap>
<drv mrk="albert1.A0o">
  <kap><tld/>o</kap>
  <snc mrk="albert1.A0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Provinco de <ref tip="malprt" cel="kanad.K0o.GEOG">Kanado</ref>
      en &gcirc;ia sud-okcidenta parto, &ccirc;e la pacifika maro:
      <ekz>
        niaj membroj trovi&gcirc;as en diversaj partoj de Kanado:
        po tri en Ontario kaj Brita Kolumbio,
        kaj po unu en <tld/>o kaj Saska&ccirc;evano
        <fnt>
          <aut>B. Arthur</aut>,
          <vrk>La Membraro</vrk>,
          <lok><url ref="http://www.esperanto.ca/jek/ja17.html">
          Juna Alumeto, 1998-06 (17)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="albert1.0o.RIM_HOM">
    La provincon oni nomis
    honore al <em>Louise Caroline Alberta</em>
    de la brita re&gcirc;a familio, do per la homnomo
    <ref tip="hom" cel="albert.0o">Alberto</ref>.
  </rim>
  <trd lng="bg">&c_A;&c_l;&c_b;&c_malmol;&c_r;&c_t;&c_a;<klr tip="amb"> (&c_K;&c_a;&c_n;&c_a;&c_d;&c_a;)</klr></trd>
  <trd lng="de">Alberta<klr tip="amb"> (Kanada)</klr></trd>
  <trd lng="en">Alberta<klr> (Canada)</klr></trd>
  <trd lng="fr">Alberta<klr> (Canada)</klr></trd>
</drv>
</art>

