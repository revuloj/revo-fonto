<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: takipet.xml,v 1.2 2019/10/18 17:10:20 revo Exp $">
<kap>
  <rad>takipet</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="takipet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="takipet.0o.ZOO">
    <dif>
      <ref tip="dif" cel="fregat1.0o.ZOO">Fregato</ref>:
      <ekz>
        oni vidis <klr>[&#8230;]</klr> supre en la aero la nigran <tld/>on
        <fnt>
          <aut>H. Bernardin de Saint-Pierre, trad. H. Hodler</aut>, 
          <vrk><url ref="http://www.luin.se/inko/180-1.pdf">
          Pa&ubreve;lo kaj Virginio</url></vrk>,
          <lok>1905</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="takipet.0o.RIM_ETIM">
    La&ubreve; malnova scienca nomo &leftquot;<nom>Tachypetes</nom>&rightquot;.
  </rim>
  <!-- prefere ne traduku izolitan arkaikan formon krom per vialingva
  arkaika&jcirc;o -->
</drv>
</art>

