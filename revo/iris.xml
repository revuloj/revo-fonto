<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: iris.xml,v 1.25 2023/11/19 10:22:14 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>iris</rad>/o
</kap>
<drv mrk="iris.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Ronda, kunstre&ccirc;ebla, diverskolora <ref tip="super" cel="membra.0o.ANA">membrano</ref>
      en la <ref tip="malprt" cel="okul.0o.ANA">okulo</ref>globo, kun centra
      truo por enlasi la lumradiojn:
      <ekz>
        kiam okazas la vera ekbrilo, la okulfono kaj &gcirc;ia koloro jam estas preska&ubreve;
        nevideblaj, ka&scirc;itaj de la okula <tld/>o, kovranta la pupilon
        <fnt>
          <bib>Monato</bib>
          <aut>J&aacute;nos S&aacute;rk&ouml;zi</aut>
          <vrk><url ref="&Monato;/2005/008401.php">La fenomeno de ru&gcirc;aj okuloj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kapti biometriajn informojn, ekzemple <klr>[&#x2026;]</klr> skanitan okulan <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2005/008679.php">Karto al la estonteco</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la flava <tld/>o (tiu de turfalko estus malhela), la hela linieto super la
        okuloj, la dorsa koloro, la la&ubreve;lar&gcirc;e striita ventro estas tipaj markoj de
        e&ubreve;ropa nizo
        <fnt>
          <bib>Monato</bib>
          <aut>Thierry Tailhades</aut>
          <vrk><url ref="&Monato;/2012/010805.php">Erariga foto (2)</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/c/ce/Human_eye_close_up%2C_anterior_view.jpg/360px-Human_eye_close_up%2C_anterior_view.jpg" prm="CC-BY-SA-4.0">bruna homa iriso
        <fnt>
          <aut>Rapidreflex</aut>
          <lok><url ref="&WCW;/File:Human_eye_close_up,_anterior_view.jpg">https://commons.wikimedia.org/wiki/File:Human_eye_close_up,_anterior_view.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_a;&c_l;&c_o;&c_n;&c_k;&c_a; &c_v;&c_ja;&c_s;&c_jo;&c_l;&c_k;&c_a;&c_v;&c_a;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_jo;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_i;&c_r;&c_i;&c_s;</trd>
  <trd lng="cs">duhovka</trd>
  <trdgrp lng="de">
    <trd>Iris</trd>,
    <trd>Regenbogenhaut</trd>
  </trdgrp>
  <trd lng="en">iris</trd>
  <trd lng="es">iris</trd>
  <trd lng="fr">iris<klr tip="ind"> (de l'&oelig;il)</klr></trd>
  <trdgrp lng="hu">
    <trd>sziv&aacute;rv&aacute;nyh&aacute;rtya</trd>,
    <trd>&iacute;risz <klr tip="ind">(szem&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="id">iris</trd>
  <trd lng="it">iride<klr> (anat.)</klr></trd>
  <trd lng="ja">&#x8679;&#x5F69;  <pr>&#x3053;&#x3046;&#x3055;&#x3044;</pr></trd>
  <trdgrp lng="nl">
    <trd>iris<klr tip="ind"> (in oog)</klr></trd>,
    <trd>regenboogvlies</trd>
  </trdgrp>
  <trd lng="pl">t&eogonek;cz&oacute;wka<klr> (oka)</klr></trd>
  <trd lng="pt">&iacute;ris<klr tip="amb"> (anatomia humana)</klr></trd>
  <trd lng="ru">&c_r;&c_a;&c_d;&c_u;&c_zh;&c_n;&c_a;&c_ja; &c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a;
  <klr>(&c_g;&c_l;&c_a;&c_z;&c_a;)</klr></trd>
  <trd lng="sk">d&uacute;hovka oka</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_a;&c_j;&c_d;&c_u;&c_zh;&c_n;&c_a; &c_o;&c_b;&c_o;&c_l;&c_o;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_ib;&c_r;&c_i;&c_s;</trd>
  </trdgrp>
  <trd lng="zh">&#x8679;&#x819C; <pr>h&#xF3;ngm&#xF2;</pr></trd>
</drv>
</art>
