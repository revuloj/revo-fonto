  </trdgrp>
  <trdgrp lng="nl">
    <trd>iris<klr tip="ind"> (in oog)</klr></trd>,
    <trd>regenboogvlies</trd>
  </trdgrp>
  <trd lng="pl">t&eogonek;cz&oacute;wka<klr> (oka)</klr></trd>
  <trd lng="ru">&c_r;&c_a;&c_d;&c_u;&c_zh;&c_n;&c_a;&c_ja; &c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a; <klr>(&c_g;&c_l;&c_a;&c_z;&c_a;)</klr></trd>
</drv>
</art>
