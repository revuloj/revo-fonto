<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rankor.xml,v 1.9 2020/02/08 07:10:17 revo Exp $">
<kap>
  <rad>rankor</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="rankor.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">POE</uzo>
  <snc>
    <dif>
      Da&ubreve;ra postsento kontra&ubreve; iu pro ne kompensita ofendo;
      nepardonemo.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>mstiv&aacute; posedlost</trd>,
    <trd>pomstychtivost</trd>,
    <trd>z&aacute;&scaron;&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Groll</trd>,
    <trd>Hass</trd>,
    <trd>Verbitterung</trd>
  </trdgrp>
  <trd lng="en">resentment</trd>
  <trdgrp lng="fr">
    <trd>ranc&oelig;ur</trd>,
    <trd>rancune</trd>,
    <trd>ressentiment</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>neheztel&eacute;s</trd>,
    <trd>s&eacute;relem</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>wrok</trd>,
    <trd>rancune</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>rancor</trd>,
    <trd>m&aacute;goa</trd>,
    <trd>ressentimento</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_r;&c_je;&c_ch;&c_mol;<klr tip="amb"> (&c_o;&c_t; &c_o;&c_b;&c_i;&c_d;&c_y;)</klr></trd>,
    <trd>&c_z;&c_l;&c_o;&c_p;&c_a;&c_m;&c_ja;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_l;&c_o;&c_b;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pomstychtivos&tcaron;</trd>,
    <trd>posadnutos&tcaron; pomstou</trd>
  </trdgrp>
</drv>

<drv mrk="rankor.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">POE</uzo>
  <snc>
    <dif>
      Amare postsenti pro ofendo:
      <ekz>
        aliaj, malgrandanimaj, &ccirc;iam <tld/>is kontra&ubreve; la knabinoj
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 58a, p. 279a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">prahnout po pomst&ecaron;</trd>
  <trdgrp lng="fr">
    <trd>en <ind>vouloir</ind> &agrave;</trd>,
    <trd>&eacute;prouver du <ind>ressentiment</ind></trd>
  </trdgrp>
  <trd lng="hu">neheztel</trd>
  <trdgrp lng="nl">
    <trd>wrok voelen</trd>,
    <trd>rancune voelen</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_m;&c_n;&c_i;&c_t;&c_mol; <ind>&c_o;&c_b;&c_i;&c_d;&c_u;</ind></trd>
  <trd lng="sk">prahn&uacute;&tcaron; po pomste</trd>
</drv>
</art>

