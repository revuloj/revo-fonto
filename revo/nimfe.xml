<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>nimfe</rad>/o
</kap>
<drv mrk="nimfe.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="nimfe.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Nymphaea</trd>),
      akva vegeta&jcirc;o kun lar&gcirc;aj folioj kaj blankaj floroj:
      <ekz>
        <tld/>o estas sankta floro &ccirc;e egiptoj 
        <klr>(<ref tip="sub" cel="lotus.0o.akvoplantoj">lotuso</ref>)</klr>
      </ekz>. 
    </dif>
  </snc>
  <trd lng="de">Seerose</trd>
</drv>
<drv mrk="nimfe.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj akvovegeta&jcirc;oj
      (<trd lng="la">Nymphaeaceae</trd>).
    </dif>
    <ref tip="sub" cel="nufar.0o">nufaro</ref>
  </snc>
  <trd lng="de">Seerosengew&auml;chse</trd>
</drv>
</art>
