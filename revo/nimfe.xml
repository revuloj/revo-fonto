<art mrk="$Id: nimfe.xml,v 1.23 2022/03/24 10:41:39 revo Exp $">
<kap>
  <rad>nimfe</rad>/o
</kap>
<drv mrk="nimfe.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="nimfea.0oj"><tld/>acoj</ref>
      (<trd lng="la">Nymphaea</trd>),
      akva vegeta&jcirc;o kun lar&gcirc;aj folioj kaj blankaj floroj:
      <ekz>
        <tld/>o estas sankta floro &ccirc;e egiptoj
        <klr>(<ref tip="sub" cel="lotus.0o.akvoplantoj">lotuso</ref>)</klr>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_d;&c_z;&c_ja;&c_n;&c_ib;&c_c;&c_a;</trd>,
    <trd>&c_g;&c_a;&c_r;&c_l;&c_a;&c_ch;&c_y;&c_k;</trd>,
    <trd>&c_l;&c_ib;&c_l;&c_je;&c_ja; &c_v;&c_a;&c_d;&c_z;&c_ja;&c_n;&c_a;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>lekn&iacute;n</trd>,
    <trd>lotos</trd>,
    <trd>nymfea</trd>
  </trdgrp>
  <trd lng="de">Seerose</trd>
  <trd lng="el">&ny;&omikron;&ypsilon_ton;&phi;&alfa;&rho;&omikron;</trd>
  <trdgrp lng="en">
    <trd>white waterlily</trd>,
    <trd>white water lily</trd>,
    <trd>waterlily</trd>,
    <trd>water lily</trd>
  </trdgrp>
  <trd lng="fr">lotus</trd>
  <trdgrp lng="hu">
    <trd>t&uuml;nd&eacute;rr&oacute;zsa</trd>,
    <trd>tavir&oacute;zsa</trd>,
    <trd>v&iacute;zir&oacute;zsa</trd>
  </trdgrp>
  <trd lng="nl">waterlelie</trd>
  <trdgrp lng="pl">
    <trd>grzybienie</trd>,
    <trd>lilia wodna</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>ninf&eacute;ia</trd>,
    <trd>nen&uacute;far</trd>,
    <trd>bandeja-d'&aacute;gua</trd>    
  </trdgrp>
  <trdgrp lng="ro">
    <trd>nymphaea</trd>,
    <trd>nuf&abreve;r</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_v;&c_sh;&c_i;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_n;&c_i;&c_m;&c_f;&c_je;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>lekno</trd>,
    <trd>lotos</trd>
  </trdgrp>
  <trd lng="uk">&#x43B;&#x430;&#x442;&#x430;&#x442;&#x442;&#x44F;</trd>
</drv>
</art>
