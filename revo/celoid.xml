<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: celoid.xml,v 1.11 2023/08/31 10:10:25 revo Exp $">
<kap>
  <rad>celoidin</rad>/o
</kap>

<drv mrk="celoid.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="kolodi.0o">Kolodio</ref>:
      <ekz>
        <tld/>o estas solva&jcirc;o de piroksilino, t.e. celuloza nitrato,
        anka&ubreve; nomata <ref tip="prt" cel="koton.fulm0o">fulmkotono</ref>,
        en etero a&ubreve; alkoholo, uzata ekz. por prepari specimenojn por mikroskopado.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_e;&c_l;&c_ja;&c_ib;&c_d;&c_y;&c_n;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_o;&c_d;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="ca">cel&middot;lo&iuml;dina</trd>
  <trd lng="de">Celloidin</trd>
  <trd lng="hu">celloidin</trd>
  <trd lng="ja">&#x30BB;&#x30ED;&#x30A4;&#x30B8;&#x30F3;</trd>
  <trd lng="pl">celoidyna</trd>
  <trd lng="ru">&c_c;&c_je;&c_l;&c_l;&c_o;&c_i;&c_d;&c_i;&c_n;</trd>
</drv>
</art>

