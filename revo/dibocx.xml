    <trd>Prasser</trd>,
    <trd>ausschweifender Mensch</trd>,
    <trd>liederlicher Mensch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>debauchee</trd>,
    <trd>libertine</trd>,
    <trd>licentious person</trd>,
    <trd>glutton</trd>
  </trdgrp>
  <trdgrp lng="fr">
