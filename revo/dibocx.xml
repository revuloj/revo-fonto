  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_p;&c_u;&c_s;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_d;&c_e;&c_b;&c_a;&c_sh;&c_y;&c_r;</trd>,
