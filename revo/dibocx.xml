<kap>
  <ofc>*</ofc>
  <rad>dibo&ccirc;</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="dibocx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="dibocx.0i.MOR">
    <dif>
      Malmodere man&gcirc;i, trinki, seksumi:
      <ekz>
        iuj <tld/>as, kartluda&ccirc;as a&ubreve; pre&gcirc;as al dio
        kaj
        tiel pasigas sian liberan tempon
        <fnt><bib>Lanti</bib>, <lok>Denove pri dubemo</lok></fnt>;
      </ekz>
      <ekz>
        la vic-re&gcirc;o <klr>[...]</klr> ludis la rolon de <tld/>anta
        junulo
        <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 6</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la vento <tld/>is <klr>(furiozis, senbridi&gcirc;is)</klr> sur
        la
        kampoj<fnt>B</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="orgi.0o">orgio</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>schwelgen</trd>,
    <trd>prassen</trd>,
    <trd>ausschweifend leben</trd>,
    <trd>liederlich leben</trd>,
    <trd>ein ausschweifendes Leben f&uuml;hren</trd>
  </trdgrp>
  <trd lng="en">debauch</trd>
  <trdgrp lng="fr">
    <trd>faire la <ind>noce</ind></trd>,
    <trd>faire des <ind>exc&egrave;s</ind></trd>,
    <trd>se <ind>d&eacute;vergonder</ind></trd>
  </trdgrp>
  <trd lng="he">&lamed;&he;&tav;&he;&vav;&lamed;&lamed;</trd>
  <trdgrp lng="ja">
    <trd>&#x653E;&#x8569;&#x3059;&#x308B;  <pr>&#x306F;&#x306A;&#x8569;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x9152;&#x8272;&#x306B;&#x3075;&#x3051;&#x308B;  <pr>&#x3055;&#x3051;&#x3044;&#x308D;&#x306B;&#x3075;&#x3051;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>by&cacute; <ind>rozpustnym</ind></trd>,
    <trd>by&cacute; <ind>rozwi&aogonek;z&lstroke;ym</ind></trd>,
    <trd>by&cacute; <ind>wyuzdanym</ind></trd>,
    <trd>hula&cacute;</trd>,
    <trd>&lstroke;ajdaczy&cacute; si&eogonek;</trd>,
    <trd>puszcza&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>debochar</trd>,
    <trd>viver no <ind>deboche</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_je;&c_b;&c_o;&c_sh;&c_i;&c_r;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_je;&c_s;&c_t;&c_i;
    &c_r;&c_a;&c_s;&c_p;&c_u;&c_t;&c_n;&c_y;&c_j;
    &c_o;&c_b;&c_r;&c_a;&c_z; &c_zh;&c_i;&c_z;&c_n;&c_i;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_r;&c_a;&c_t;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_u;&c_l;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_u;&c_l;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_je;&c_s;&c_t;&c_i; &c_r;&c_o;&c_z;&c_p;&c_u;&c_s;&c_n;&c_i;&c_j; &c_s;&c_p;&c_o;&c_s;&c_ib;&c_b; &c_zh;&c_i;&c_t;&c_t;&c_ja;</trd>
  </trdgrp>
  <trd lng="zh">&#x82B1;&#x5929;&#x9152;&#x5730; <pr>hu&#x101;ti&#x101;nji&#x1D4;d&#xEC;</pr></trd>
</drv>

<drv mrk="dibocx.0o">
  <kap><tld/>o</kap>
  <snc mrk="dibocx.0o.MOR">
    <dif>
      Konduto de <tld/>anto:
      <ekz>
        tie bruis ridado kaj bojado de hundoj, tie regis <tld/>ado kaj
        drinkado
        <fnt><bib>Fab3</bib>, <lok>&Ccirc;io sur sian &gcirc;ustan
        lokon</lok></fnt>;
      </ekz>
      <ekz>
        la tagoj pasadis por ili en senhontaj <tld/>oj por la honoro de
        la diino <nom>Astarte</nom>
        <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 6</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Schwelgerei</trd>,
    <trd>Prasserei</trd>,
    <trd>Ausschweifung</trd>,
    <trd>Orgie</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>debauch</trd>,
    <trd>debauchery</trd>,
    <trd>orgy</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;bauche</trd>
  <trd lng="he">&he;&vav;&#x5B9;&lamed;&#x5B5;&lamed;&vav;&#x5BC;&tav;</trd>
  <trdgrp lng="ja">
    <trd>&#x653E;&#x8569;  <pr>&#x307B;&#x3046;&#x3068;&#x3046;</pr></trd>,
    <trd>&#x904A;&#x8569;  <pr>&#x3086;&#x3046;&#x3068;&#x3046;</pr></trd>,
    <trd>&#x9053;&#x697D;  <pr>&#x307F;&#x3061;&#x304C;&#x304F;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rozpusta</trd>,
    <trd>hulanka</trd>,
    <trd>nierz&aogonek;d</trd>
    <trd>rozwi&aogonek;z&lstroke;o&sacute;&cacute;</trd>,
    <trd>wyuzdanie</trd>,
  </trdgrp>
  <trd lng="ro">anarhie</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_b;&c_o;&c_sh;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_g;&c_u;&c_l;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_u;&c_l;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_r;&c_a;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_u;&c_l;&c_mol;&c_b;&c_a;</trd>,
    <trd>&c_g;&c_u;&c_l;&c_mol;&c_n;&c_ja;</trd>,
    <trd>&c_g;&c_u;&c_l;&c_ja;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_p;&c_u;&c_s;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#x72C2;&#x6B22; <pr>ku&#xE1;nghu&#x101;n</pr></trd>
</drv>

<drv mrk="dibocx.0a">
  <kap><tld/>a</kap>
  <snc mrk="dibocx.0a.MOR">
    <dif>
      Karakterizata per <tld/>o:
      <ekz>
        nia princo, kvankam vivega kaj iom <tld/>a, kiel ordinare la
        junuloj, scias tamen respekti la konsilojn de l' sa&gcirc;uloj
        <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 11</lok></fnt>;
      </ekz>
      <ekz>
        kiom da virinoj la direktoro konscie a&ubreve; nekonscie
        infektis dum
        sia <tld/>a vivo
        <fnt><bib>UrdHadda</bib>, <lok>p. 23</lok></fnt>;
      </ekz>
      <ekz>
        malpiuloj <klr>[&scirc;an&gcirc;is]</klr> la gracon de nia Dio
        en <ind><tld/>eco</ind>n
        <fnt><bib>NT</bib>, <lok>&Jud; 4</lok></fnt>.
        <trdgrp lng="fr">
          <trd>dissolution</trd>,
          <trd>d&eacute;pravation</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>schwelgerisch</trd>,
    <trd>ausschweifend</trd>,
    <trd>liederlich</trd>
  </trdgrp>
  <trd lng="en">debaucherous</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;bauch&eacute;<klr tip="ind"> (adj.)</klr></trd>,
    <trd>dissolu<klr tip="ind"> (d&eacute;prav&eacute;)</klr></trd>,
    <trd>d&eacute;prav&eacute;</trd>,
    <trd>de <ind>d&eacute;bauche</ind></trd>
  </trdgrp>
  <trd lng="he">&he;&vav;&#x5B9;&lamed;&#x5B5;&lamed;</trd>
  <trd lng="ja">&#x653E;&#x8569;&#x306E;  <pr>&#x306F;&#x306A;&#x8569;&#x306E;</pr></trd>
  <trdgrp lng="pl">
    <trd>rozpustny</trd>,
    <trd>rozwi&aogonek;&zacute;le</trd>,
    <trd>wyuzdany</trd>,
    <trd>hulaszczy</trd>,
    <trd>rozpasany</trd>
    <trd>nierz&aogonek;dny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_g;&c_u;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_r;&c_a;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_u;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="zh">&#x8F15;&#x72C2; <pr>q&#x12B;ngku&#xE1;ng</pr></trd>
</drv>

<drv mrk="dibocx.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="dibocx.0ajxo.MOR">
    <dif>
      Troa man&gcirc;a a&ubreve; seksa &gcirc;uado:
      <ekz>
        lia fian&ccirc;i&gcirc;o tenas lin malproksime de &ccirc;iuj
        <tld/>a&jcirc;oj
        <fnt><bib>Fab4</bib>, <lok>Sterkskarabo</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="orgi.0o.MOR">orgio</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Ausschweifung</trd>,
    <trd>Gelage</trd>,
    <trd>Liederlichkeit</trd>,
    <trd>Prasserei</trd>,
    <trd>Schwelgerei</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>binge</trd>,
    <trd>revelry</trd>
  </trdgrp>
  <trd lng="fr">exc&egrave;s<klr tip="ind"> (d&eacute;bauche)</klr></trd>
  <trd lng="he">&he;&vav;&#x5B9;&lamed;&#x5B5;&lamed;&vav;&#x5BC;&tav;</trd>
  <trdgrp lng="pl">
    <trd>rozpusta</trd>,
    <trd>hulanka</trd>,
    <trd>nierz&aogonek;d</trd>
    <trd>orgia</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_b;&c_o;&c_sh;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_n;&c_d;&c_a;&c_l;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x72C2;&#x95F9; <pr>ku&#xE1;ngn&#xE0;o</pr></trd>,
    <trd>&#x72C2;&#x6B22; <pr>ku&#xE1;nghu&#x101;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="dibocx.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="dibocx.0ulo.MOR">
    <dif>
      Tiu, kiu ordinare <tld/>as:
      <ekz>
        <nom>Kant</nom> sentis, ke li estas
        malsupera je rapidspriteco apud tiu senzorga <tld/>ulo
        <fnt><bib>UrdHadda</bib>, <lok>p. 12a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>W&uuml;stling</trd>,
    <trd>Prasser</trd>,
    <trd>ausschweifender Mensch</trd>,
    <trd>liederlicher Mensch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>debauchee</trd>,
    <trd>libertine</trd>,
    <trd>licentious person</trd>,
    <trd>glutton</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;bauch&eacute;<klr tip="ind"> (subst.)</klr></trd>,
    <trd>noceur</trd>,
    <trd>viveur</trd>
  </trdgrp>
  <trd lng="he">&shin;&#x5C1;&vav;&#x5B9;&bet;&#x5B8;&bet;</trd>
  <trd lng="ja">&#x653E;&#x8569;&#x8005;  <pr>&#x307B;&#x3046;&#x3068;&#x3046;&#x3082;&#x306E;</pr></trd>
  <trdgrp lng="pl">
    <trd>rozpustnik</trd>,
    <trd>hulaka</trd>,
    <trd>lowelas</trd>,
    <trd>nierz&aogonek;dnica<klr> {f.}</klr></trd>,
    <trd>birbant<klr> (przest.)</klr></trd>,
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_v;&c_r;&c_a;&c_t;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_u;&c_t;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_g;&c_u;&c_l;&c_ja;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_u;&c_l;&c_mol;&c_t;&c_ja;&c_j;</trd>,
    <trd>&c_g;&c_u;&c_l;&c_mol;&c_v;&c_ib;&c_s;&c_a;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_p;&c_u;&c_s;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="zh">&#x653E;&#x8361;&#x8005; <pr>f&#xE0;ngd&#xE0;ngzh&#x11B;</pr></trd>
</drv>

<drv mrk="dibocx.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="dibocx.0igi.MOR">
    <dif>
      Fari, ke iu <tld/>u.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>zu Ausschweifungen verleiten</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>lead to debauchery</trd>,
    <trd>make debauched</trd>,
    <trd>corrupt</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;praver</trd>,
    <trd>d&eacute;vergonder</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8A98;&#x60D1;&#x3059;&#x308B;  <pr>&#x3086;&#x3046;&#x308F;&#x304F;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5815;&#x843D;&#x3055;&#x305B;&#x308B;  <pr>&#x3060;&#x3089;&#x304F;&#x3055;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zdeprawowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_v;&c_r;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_r;&c_a;&c_shch;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="dibocx.for0i">
  <kap>for<tld/>i, <var><kap>tra<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="dibocx.for0i.MOR">
    <dif>
      Perdi per <tld/>o:
      <ekz>
        <klr>[&ccirc;]</klr>ion &ccirc;i tiel facile for<tld/>i &ccirc;e gaja festeno
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        iafoje li tra<tld/>as &ccirc;ion, &gcirc;is la lasta &ccirc;emizo, tiel ke sur li restas nur
        surtuteto
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        do trezoro mia fluos en <klr>[&#x2026;]</klr> truplenajn po&scirc;ojn // <klr>[&#x2026;]</klr> Li
        for<tld/>os // Sed la&ubreve; kiu rajto
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 6 (56)</lok>
        </fnt>?
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="posx.el0igi.FIG">elpo&scirc;igi<sncref/></ref>,
      <ref cel="spez.el0i">elspezi</ref>,
      <ref cel="sxpar.mal0i">mal&scirc;pari</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>verjubeln</trd>,
    <trd>verprassen</trd>,
    <trd>verludern</trd>,
    <trd>verjuxen</trd>,
    <trd>vergeuden</trd>,
    <trd>durchbringen <klr>(Besitz)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>squander</trd>,
    <trd>dissipate</trd>,
    <trd>fritter away</trd>,
    <trd>waste</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>dilapider<klr tip="ind"> (dans des plaisirs)</klr></trd>,
    <trd>flamber<klr tip="ind"> (dans des plaisirs)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x8569;&#x5C3D;&#x3059;&#x308B; <pr>&#x3068;&#x3046;&#x3058;&#x3093;&#x3059;&#x308B;</pr></trd>
  <trdgrp lng="pl">
    <trd>przehula&cacute;</trd>,
    <trd>przepu&sacute;ci&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_k;&c_u;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;
    <klr>(&c_d;&c_je;&c_n;&c_mol;&c_g;&c_i;)</klr></trd>
  </trdgrp>
  <trd lng="uk">&c_p;&c_r;&c_o;&c_g;&c_u;&c_l;&c_ja;&c_t;&c_i;</trd>
  <trd lng="zh">&#x6557;&#x5BB6; <pr>b&#xE0;iji&#x101;</pr></trd>
</drv>

<drv mrk="dibocx.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="dibocx.0ejo.MOR">
    <dif>
      Loko, kie oni povas <tld/>i
    </dif>
    <refgrp tip="vid">
      <ref cel="bordel.0o">bordelo</ref>,
      <ref cel="drink.0ejo">drinkejo</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Bumslokal</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>burdel</trd>,
      <trd>dom publiczny</trd>,
      <trd>dom schadzek</trd>
    </trdgrp>
    <trd lng="ru">&c_z;&c_l;&c_a;&c_ch;&c_n;&c_o;&c_je;
    &c_m;&c_je;&c_s;&c_t;&c_o;</trd>
  </snc>
  <trd lng="en">sleazy joint</trd>
  <trd lng="fr">lieu de d&eacute;bauche</trd>
  <trd lng="he">&he;&vav;&#x5B9;&lamed;&#x5B5;&lamed;&vav;&#x5BC;&tav;</trd>
  <trd lng="ja">&#x60AA;&#x6240;  <pr>&#x3042;&#x304F;&#x3057;&#x3087;</pr></trd>
</drv>
