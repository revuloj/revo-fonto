<drv mrk="dibocx.for0i">
  <kap>for<tld/>i, <var><kap>tra<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="dibocx.for0i.MOR">
    <dif>
      Perdi per <tld/>o:
      <ekz>
        <klr>[&ccirc;]</klr>ion &ccirc;i tiel facile for<tld/>i &ccirc;e gaja festeno
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        iafoje li tra<tld/>as &ccirc;ion, &gcirc;is la lasta &ccirc;emizo, tiel ke sur li restas nur
        surtuteto
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        do trezoro mia fluos en <klr>[&#8230;]</klr> truplenajn po&scirc;ojn // <klr>[&#8230;]</klr> Li
        for<tld/>os // Sed la&ubreve; kiu rajto
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 6 (56)</lok>
        </fnt>?
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="posx.el0igi.FIG">elpo&scirc;igi<sncref/></ref>,
      <ref cel="spez.el0i">elspezi</ref>,
      <ref cel="sxpar.mal0i">mal&scirc;pari</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_g;&c_u;&c_l;&c_ja;&c_c;&c_mol; <klr tip="amb">(&c_n;&c_a;&c_p;&c_r;. &c_g;&c_r;&c_o;&c_sh;&c_y; &c_w; &c_r;&c_a;&c_s;&c_p;&c_u;&c_s;&c_mol;&c_c;&c_je;)</klr></trd>,
    <trd>&c_p;&c_r;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_p;&#8217;&c_ja;&c_n;&c_s;&c_t;&c_v;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verjubeln</trd>,
    <trd>verprassen</trd>,
    <trd>verludern</trd>,
    <trd>verjuxen</trd>,
    <trd>vergeuden</trd>,
    <trd>durchbringen <klr>(Besitz)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>squander</trd>,
    <trd>dissipate</trd>,
    <trd>fritter away</trd>,
    <trd>waste</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>dilapider<klr tip="ind"> (dans des plaisirs)</klr></trd>,
    <trd>flamber<klr tip="ind"> (dans des plaisirs)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#34153;&#23613;&#12377;&#12427; <pr>&#12392;&#12358;&#12376;&#12435;&#12377;&#12427;</pr></trd>
