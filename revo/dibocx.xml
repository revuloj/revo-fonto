<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>dibo&ccirc;</rad>/i
</kap>
<drv mrk="dibocx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Malmodere man&gcirc;i, trinki, seksumi:
      <ekz>
        iuj <tld/>as, kartluda&ccirc;as a&ubreve; pre&gcirc;as al dio kaj
        tiel pasigas sian liberan tempon
        <fnt><bib>Lanti</bib>, <lok>Denove pri dubemo</lok></fnt>;
      </ekz>
      <ekz>
        la vic-re&gcirc;o <klr>[...]</klr> ludis la rolon de <tld/>anta junulo
        <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 6</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la vento <tld/>is <klr>(furiozis, senbridi&gcirc;is)</klr> sur la
        kampoj<fnt>B</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="orgi.0o">orgio</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>schwelgen</trd>,
     <trd>prassen</trd>,
     <trd>ausschweifend leben</trd>,
     <trd>liederlich leben</trd>,
     <trd>ein ausschweifendes Leben f&uuml;hren</trd>
  </trdgrp>
  <trd lng="en">debauch</trd>
  <trdgrp lng="fr">
    <trd>faire la <ind>noce</ind></trd>,
    <trd>faire des <ind>exc&egrave;s</ind></trd>,
    <trd>se <ind>d&eacute;vergonder</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_je;&c_b;&c_o;&c_sh;&c_i;&c_r;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_je;&c_s;&c_t;&c_i; &c_r;&c_a;&c_s;&c_p;&c_u;&c_t;&c_n;&c_y;&c_j; &c_o;&c_b;&c_r;&c_a;&c_z; &c_zh;&c_i;&c_z;&c_n;&c_i;</trd>
  </trdgrp>
</drv>
<drv mrk="dibocx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Konduto de <tld/>anto:
      <ekz>
        tie bruis ridado kaj bojado de hundoj, tie regis <tld/>ado kaj drinkado
        <fnt><bib>Fab3</bib>, <lok>&Ccirc;io sur sian &gcirc;ustan lokon</lok></fnt>;
      </ekz>
      <ekz>
        la tagoj pasadis por ili en senhontaj <tld/>oj por la honoro de la
        diino Astarte
        <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 6</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Schwelgerei</trd>,
     <trd>Prasserei</trd>,
     <trd>Ausschweifung</trd>,
     <trd>Liederlichkeit</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;bauche</trd>
</drv>
<drv mrk="dibocx.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Karakterizata per <tld/>o:
      <ekz>
        nia princo, kvankam vivega kaj iom <tld/>a, kiel ordinare la junuloj,
        scias tamen respekti la konsilojn de l' sa&gcirc;uloj
        <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 11</lok></fnt>;
      </ekz>
      <ekz>
        kiom da virinoj la direktoro konscie a&ubreve; nekonscie infektis dum
        sia <tld/>a vivo
        <fnt><bib>UrdHadda</bib>, <lok>p. 23</lok></fnt>;
      </ekz>
      <ekz>
        malpiuloj <klr>[&scirc;an&gcirc;is]</klr> la gracon de nia Dio
        en <ind><tld/>eco</ind>n
        <fnt><bib>X</bib>, <lok>&Jud; 4</lok></fnt>.
        <trdgrp lng="fr">
          <trd>dissolution</trd>,
          <trd>d&eacute;pravation</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>schwelgerisch</trd>
     <trd>ausschweifend</trd>,
     <trd>liederlich</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;bauch&eacute;<klr tip="ind"> (adj.)</klr></trd>,
    <trd>dissolu<klr tip="ind"> (d&eacute;prav&eacute;)</klr></trd>,
    <trd>d&eacute;prav&eacute;</trd>,
    <trd>de <ind>d&eacute;bauche</ind></trd>
  </trdgrp>
</drv>
<drv mrk="dibocx.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Ekscesoj en la tablaj a&ubreve; seksaj plezuroj:
      <ekz>
        lia fian&ccirc;i&gcirc;o tenas lin malproksime de &ccirc;iuj
        <tld/>a&jcirc;oj
        <fnt><bib>Fab4</bib>, <lok>Sterkskarabo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Schwelgerei</trd>,
     <trd>Prasserei</trd>,
     <trd>Ausschweifung</trd>,
     <trd>Liederlichkeit</trd>
  </trdgrp>
  <trd lng="fr">exc&egrave;s<klr tip="ind"> (d&eacute;bauche)</klr></trd>
</drv>
<drv mrk="dibocx.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Tiu, kiu ordinare <tld/>as:
      <ekz>
        Kant sentis, ke li estas
        malsupera je rapidspriteco apud tiu senzorga <tld/>ulo
        <fnt><bib>UrdHadda</bib>, <lok>p. 12</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>W&uuml;stling</trd>,
     <trd>Prasser</trd>,
     <trd>ausschweifender Mensch</trd>,
     <trd>liederlicher Mensch</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;bauch&eacute;<klr tip="ind"> (subst.)</klr></trd>,
    <trd>noceur</trd>,
    <trd>viveur</trd>
  </trdgrp>
</drv>
<drv mrk="dibocx.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari, ke iu <tld/>u.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>zu Ausschweifungen verleiten</trd>  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;praver</trd>,
    <trd>d&eacute;vergonder</trd></trdgrp> 
</drv>
<drv mrk="dibocx.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Perdi per <tld/>o:
      <ekz>
        li for<tld/>is &ccirc;ion, &gcirc;is la lasta &ccirc;emizo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>verjubeln</trd>,
     <trd>verprassen</trd>,
     <trd>verludern</trd>,
     <trd>verjuxen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>dilapider<klr tip="ind"> (dans des plaisirs)</klr></trd>,
    <trd>flamber<klr tip="ind"> (dans des plaisirs)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_k;&c_u;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol; <klr>(&c_d;&c_je;&c_n;&c_mol;&c_g;&c_i;)</klr></trd>
  </trdgrp>
</drv>
<drv mrk="dibocx.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Loko, kie oni povas <tld/>i
    </dif>
    <refgrp tip="vid">
      <ref cel="bordel.0o">bordelo</ref>,
      <ref cel="drink.0ejo">drinkejo</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Bumslokal</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>lieu de d&eacute;bauche</trd>
    </trdgrp>
    <trd lng="ru">&c_z;&c_l;&c_a;&c_ch;&c_n;&c_o;&c_je; &c_m;&c_je;&c_s;&c_t;&c_o;</trd>
  </snc>
</drv>
</art>
