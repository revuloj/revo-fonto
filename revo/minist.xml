      <bib>Monato</bib><lok>jaro 1997a, numero 2a, p. 14a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_n;&c_ib;&c_s;&c_t;&c_e;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>
  <trd lng="ca">ministeri</trd>
  <trd lng="de">Ministerium</trd>
  <trdgrp lng="en">
    <trd>ministry</trd>,
    <trd>(cabinet) <ind>department</ind></trd>
  </trdgrp>
  <trd lng="es">ministerio</trd>
  <trdgrp lng="fr">
    <trd>minist&egrave;re</trd>
  </trdgrp>
  <trd lng="hu">miniszt&eacute;rium</trd>
  <trd lng="ru">&c_m;&c_i;&c_n;&c_i;&c_s;&c_t;&c_je;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>
</art>
