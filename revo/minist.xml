<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>ministeri</rad>/o
</kap>
<drv mrk="minist.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="minist1.0ejo.administrejo">Ministrejo<sncref/></ref>:
      <ekz>
        <tld/>o pri transporto
        <fnt>
          <bib>Monato</bib>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url ref="http://www.monato.net/2007/009290.php">Busoj, biciklantoj kaj kavaliraj kutimoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o pri edukado
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="http://www.monato.net/2008/009356.php">Sistemo ne imuna</url></vrk>
        </fnt>
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="http://www.monato.net/2009/009620.php">Ferioj senferiaj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o pri enlandaj aferoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="http://www.monato.net/2011/010271.php">Malsamaj kaj disigitaj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj pri eksterlandaj aferoj kaj pri instruado
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="http://www.monato.net/2010/009959.php">Libreto lerneja</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ina <tld/>o pri fervojoj
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="http://www.monato.net/2010/010021.php">Migrado multmiliona</url></vrk>
        </fnt>,
        pri komunikado
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="http://www.monato.net/2011/010328.php">Fragmentoj de la dua mondmilito</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i reorganizis la administradon, dividante &ccirc;iun <tld/>on en
        funkciplenuman unuon kaj konsilantan unuon<fnt><aut>Donald
        Rogers</aut>: <vrk>Ekonomia miraklo a&ubreve; malsano?</vrk>,
        <bib>Monato</bib><lok>jaro 1997a, numero 2a, p. 14a</lok></fnt>;
      </ekz>
      <ekz>
        en orient-e&ubreve;ropaj landoj alkohol-konsumado kreskas: la&ubreve; la 
        litova <tld/>o pri san-protektado je 18,4 % inter 1990 kaj 2002
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="http://www.monato.net/2005/008663.php">Je via malsano</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">
&c_m;&c_ib;&c_n;&c_ib;&c_s;&c_t;&c_e;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>
  <trd lng="ca">ministeri</trd>
  <trdgrp lng="cs">
    <trd>ministerium (zast.)</trd>,
    <trd>ministerstvo <klr>(&uacute;&rcaron;ad)</klr></trd>
  </trdgrp>
  <trd lng="de">Ministerium</trd>
  <trdgrp lng="en">
    <trd>ministry</trd>,
    <trd>(cabinet) <ind>department</ind></trd>
  </trdgrp>
  <trd lng="es">ministerio</trd>
  <trdgrp lng="fa">
    <trd>&a_w;&a_z;&a_A;&a_r;&a_t;</trd>,
    <trd>&a_w;&a_z;&a_A;&a_r;&a_t;&a_kh;&a_A;&a_n;&a_h;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>minist&egrave;re</trd>
  </trdgrp>
  <trd lng="hu">miniszt&eacute;rium</trd>
  <trd lng="it">ministero</trd>
  <trd lng="nl">ministerie</trd>
  <trd lng="pl">ministerstwo</trd>
  <trd lng="ru">
&c_m;&c_i;&c_n;&c_i;&c_s;&c_t;&c_je;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>
  <trdgrp lng="sk">
    <trd><klr>(aj budova)</klr></trd>,
    <trd>ministerstvo</trd>
  </trdgrp>
</drv>
</art>
