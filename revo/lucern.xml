<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lucern.xml,v 1.18 2024/01/14 08:27:37 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>lucern</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="lucern.0o">
  <kap><tld/>o</kap>
  <snc mrk="lucern.0o.LUM">
    <dif>
      Pendanta
      <ref tip="super" cel="lamp.0o.LUM">lampo</ref>,
      speciale en pre&gcirc;ejo:
      <ekz>
        ili alportu al vi oleon <klr>[&#8230;]</klr> por lumigado, por ke <tld/>o brulu &ccirc;iam
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 27:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i tie pendis la ar&gcirc;enta <tld/>o de memoro kaj
        trovi&gcirc;is anka&ubreve; &ccirc;irka&ubreve;kradita estrado
        <fnt>
          <aut>H. Heine, trad. L. L. Zamenhof</aut>
          <vrk>La Rabeno de Ba&hcirc;ara&hcirc;</vrk>,
          <lok>1929</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj lumigis la grandan triumfan arkon
        <fnt>
          <bib>IKr</bib>
          <lok>Neniam Plu!</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        defiantaj la lumon de la unuopa <tld/>o, kiu estis levi&gcirc;anta super la urbo, la
        lun<tld/>on
        <fnt>
          <bib>MkM</bib>
          <lok>26</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ib;&c_h;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_s;&c_mol;&c_v;&c_ja;&c_c;&c_ib;&c_l;&c_mol;&c_n;&c_ja;</trd>,
    <trd>&c_l;&c_ja;&c_m;&c_p;&c_a;&c_d;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd><ofc>2</ofc>Ampel <klr>(H&auml;ngeleuchte)</klr></trd>,
     <trd>H&auml;ngeleuchte</trd>,
     <trd>ewiges <ind>Licht</ind></trd>
  </trdgrp>
  <trd lng="en">hanging lamp in church</trd>
  <trd lng="el">&phi;&epsilon;&gamma;&gamma;&jota_ton;&tau;&eta;&sigma_fina;</trd>
  <trd lng="es">lucerna</trd>
  <trd lng="fr"><ofc>2</ofc>lampe <klr>(d&#8217;&eacute;glise)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#12388;&#12426;&#12521;&#12531;&#12503;  <pr>&#12388;&#12426;&#12425;&#12435;&#12407;</pr></trd>,
    <trd>&#20625;&#20986;&#12375;&#12383;&#20154;&#29289;  <pr>&#12369;&#12387;&#12375;&#12421;&#12388;&#12375;&#12383;&#12376;&#12435;&#12406;&#12388;</pr></trd>
  </trdgrp>
  <trd lng="nl">hanglamp</trd>
  <trdgrp lng="pl">
    <trd>kaganek</trd>,
    <trd>lampa wisz&aogonek;ca</trd>
  </trdgrp>
  <trd lng="pt">lucerna</trd>
  <trdgrp lng="ro">
    <trd>opai&tcomma;</trd>,
    <trd>lampa pandantiv</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_f;&c_o;&c_n;&c_a;&c_r;&c_mol;</trd>,
    <trd>&c_s;&c_v;&c_je;&c_t;&c_i;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_l;&c_a;&c_m;&c_p;&c_a;&c_d;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_l;&c_a;&c_m;&c_p;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_s;&c_v;&c_ib;&c_t;&c_i;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
