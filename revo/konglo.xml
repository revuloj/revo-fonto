<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>konglomerat</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="konglo.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Maso konsistanta el mineralaj rompita&jcirc;oj nature kuncementitaj.
    </dif>
    <trd lng="hu">konglomer&aacute;tum</trd>
  </snc>
  <snc>
    <dif>
      Maso konsistanta el kunigitaj ne samspecaj partoj.
    </dif>
    <trdgrp lng="hu">
      <trd>konglomer&aacute;tum</trd>,
      <trd>halmaz <klr tip="ind"> (rendezetlen)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_g;&c_l;&c_ja;&c_m;&c_e;&c_r;&c_a;&c_t;</trd>
  <trd lng="ca">conglomerat</trd>
  <trd lng="de">Konglomerat</trd>
  <trd lng="en">conglomerate</trd>
  <trd lng="es">conglomerado</trd>
  <trd lng="nl">conglomeraat</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_g;&c_l;&c_o;&c_m;&c_je;&c_r;&c_a;&c_t;</trd>
</drv>
</art>
