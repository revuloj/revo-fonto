<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pavim</rad>/o
</kap>
<drv mrk="pavim.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Tega&jcirc;o el &scirc;tono, ligno, asfalto ks etendita sur tero de strato,
      vestiblo kc.
    </dif>
  </snc>                    
  <trd lng="be">&c_b;&c_r;&c_u;&c_k;</trd>
  <trd lng="br">pavez<klr tip="ind"> (hollad pavezio&ugrave; ur straed)</klr></trd>
  <trd lng="de">Pflaster</trd>   
  <trd lng="fr">pavage</trd>
  <trd lng="es">pavimento</trd>
  <trdgrp lng="hu">
    <trd>k&ouml;vezet</trd>,
    <trd>&uacute;tburkolat</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>plaveisel</trd>,
    <trd>bestrating</trd>
  </trdgrp>
  <trd lng="pt">pavimento</trd>
  <trd lng="ru">&c_m;&c_o;&c_s;&c_t;&c_o;&c_v;&c_a;&c_ja;</trd>
</drv>

<drv mrk="pavim.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Tegi per <tld/>o:
      <ekz>
        en nia vila&gcirc;o ne &ccirc;iuj stratoj estas
<tld/>itaj<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        per promesoj estas <tld/>ita la infero
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>                          
  <trd lng="be">&c_b;&c_r;&c_u;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="br">paveza&ntilde;</trd>
  <trd lng="de">pflastern</trd>   
  <trd lng="es">pavimentar</trd>
  <trd lng="fr">paver</trd>
  <trdgrp lng="hu">
    <trd>kik&ouml;vez</trd>,
    <trd>burkol <klr tip="ind"> (utat)</klr></trd>,
    <trd>flaszteroz</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>plaveien</trd>,
    <trd>bestraten</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_o;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="pavim.0ero">
  <kap><tld/>ero</kap>
  <snc>
    <dif>
      &Ccirc;iu aparta peco konsistiganta <tld/>on.
    </dif>
  </snc>                           
  <trd lng="br">pavez</trd>
  <trd lng="de">Pflasterstein</trd>   
  <trd lng="es">adoqu&iacute;n</trd>
  <trd lng="fr">pav&eacute;</trd>
  <trdgrp lng="hu">
    <trd>burkolatk&odblac;</trd>,
    <trd>flaszterk&odblac;</trd>
  </trdgrp>
  <trd lng="nl">straatsteen</trd>
  <trd lng="ru">&c_b;&c_u;&c_l;&c_y;&c_zh;&c_n;&c_i;&c_k;
<klr>(&c_i;&c_z; &c_m;&c_o;&c_s;&c_t;&c_o;&c_v;&c_o;&c_j;)</klr></trd>
</drv>
</art>
