<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pavim</rad>/o
</kap>
<drv mrk="pavim.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Tega&jcirc;o el &scirc;tono, ligno, asfalto ks etendita sur tero de strato,
      vestiblo kc.
    </dif>
  </snc>
  <trd lng="de">Pflaster</trd>
  <trdgrp lng="nl">
    <trd>plaveisel</trd>,
    <trd>bestrating</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_o;&c_s;&c_t;&c_o;&c_v;&c_a;&c_ja;</trd>
</drv>

<drv mrk="pavim.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Tegi per <tld/>o:
      <ekz>
        en nia vila&gcirc;o ne &ccirc;iuj stratoj estas <tld/>itaj<fnt>B</fnt>;
      </ekz>
      (f)
      <ekz>
        per promesoj estas <tld/>ita la infero<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">pflastern</trd>
  <trdgrp lng="nl">
    <trd>plaveien</trd>,
    <trd>bestraten</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_o;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="pavim.0ero">
  <kap><tld/>ero</kap>
  <snc>
    <dif>
      &Ccirc;iu aparta peco konsistiganta <tld/>on.
    </dif>
  </snc>
  <trd lng="de">Pflasterstein</trd>
  <trd lng="nl">straatsteen</trd>
  <trd lng="ru">&c_b;&c_u;&c_l;&c_y;&c_zh;&c_n;&c_i;&c_k; <klr>(&c_i;&c_z; &c_m;&c_o;&c_s;&c_t;&c_o;&c_v;&c_o;&c_j;)</klr></trd>
</drv>
</art>
