<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pavim.xml,v 1.23 2022/07/20 22:30:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pavim</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pavim.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pavim.0o.KON">
    <uzo tip="fak">KON</uzo>
    <uzo tip="fak">TRA</uzo>
    <dif>
      <ref tip="super" cel="teg.0ajxo.tego">Tega&jcirc;o</ref> el &scirc;tono, ligno, bitumo ks
      etendita sur tero de strato, vestiblo...:
      <ekz>
        j ili ekgenuis viza&gcirc;altere sur la <tld/>o, adorklini&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 7:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de la ekstera korto
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 42:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia piedo trovis sur la strato nenion krom &scirc;limo kaj mar&ccirc;o, &ccirc;ar en tiu tempo ne
        ekzistis ankora&ubreve; &scirc;tona <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la planko en la &ccirc;ambroj prezentis duone disfositan <tld/>on el brikoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi iam vidis <tld/>batilon? tion, kion la <ind><tld/>isto</ind>j <klr>[&#8230;]</klr> uzas por
        la batebenigo de la &scirc;tona <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Du pavimbatiloj</lok>
        </fnt>;
        <trdgrp lng="de">
          <trd>Pflasterer</trd>,
          <trd>Steinsetzer</trd>
         </trdgrp>
      </ekz>
      <ekz>
        asfaltaj <tld/>oj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        brika <tld/>o
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o konsistis el negrandaj pli malpli rondaj &scirc;tonoj
        <fnt>
          <bib>ViV</bib>
          <lok>IV.</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="mozaik.0o">mozaiko</ref>
    <bld
      lok="&WCU;/thumb/6/6f/Steinsetzer_1880.jpg/549px-Steinsetzer_1880.jpg"
      prm="PD"
      >Laborantaj <tld/>istoj je 1880
        <fnt>
          <aut>Anonima artisto, http://www.digibib.tu-bs.de/?docid=00000286</aut>
          <lok><url ref="&WCW;/File:Steinsetzer_1880.jpg"
              >&WCW;/File:Steinsetzer_1880.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_u;&c_k;</trd>
  <trd lng="br">pavez<klr tip="ind"> (hollad pavezio&ugrave; ur
  straed)</klr></trd>
  <trd lng="cs">dla&zcaron;ba</trd>
  <trd lng="de">Pflaster</trd>
  <trdgrp lng="en">
    <trd>pavement</trd>,
    <trd>paving</trd>
  </trdgrp>
  <trd lng="es">pavimento</trd>
  <trdgrp lng="fr">
    <trd>rev&ecirc;tement routier</trd>,
    <trd>pavage<klr> (tout mat&eacute;riaux)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&ouml;vezet</trd>,
    <trd>&uacute;tburkolat</trd>
  </trdgrp>
  <trd lng="it">pavimento</trd>
  <trdgrp lng="nl">
    <trd>plaveisel</trd>,
    <trd>bestrating</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>nawierzchnia</trd>,
    <trd>bruk</trd>
  </trdgrp>
  <trd lng="pt">pavimento</trd>
  <trd lng="ro">carosabil</trd>
  <trd lng="ru">&c_m;&c_o;&c_s;&c_t;&c_o;&c_v;&c_a;&c_ja;</trd>
  <trd lng="sk">dla&zcaron;ba</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_r;&c_u;&c_k;</trd>,
    <trd>&c_b;&c_r;&c_u;&c_k;&c_ib;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="pavim.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pavim.0i.KON">
    <dif>
      <ref tip="super" cel="teg.0i">Tegi</ref> per <tld/>o:
      <ekz>
        la eniro kondukis tra <tld/>ita vestiblo
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova strata lanterno</lok>
        </fnt>;
      </ekz>
      <ekz>
        li prefere restadis &ccirc;i tie &ccirc;e la kamparanoj ol <klr>[&#8230;]</klr> <tld/>i stratojn
        en Al&gcirc;ero
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 13</lok>
        </fnt>;
      </ekz>
      <ekz>
        strato tridek pa&scirc;ojn lar&gcirc;a, <klr>[&#8230;]</klr> <tld/>ita per plataj &scirc;tonoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        la planko de la halo estis <tld/>ita per lerte kunmetitaj platoj kun desegna&jcirc;oj,
        jen geometriaj, jen arabeskaj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili iris kune sur terason, <tld/>itan per alabastro
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XLI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la unua mondmilito <klr>[&#8230;]</klr> <tld/>is la vojon por <nom>Hitler</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>Igarasi Takeo</aut>
          <vrk><url ref="&Steloj;monato2/007083.html"
              >Pedante</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        per promesoj estas <tld/>ita la infero
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_u;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="br">paveza&ntilde;</trd>
  <trd lng="cs">dl&aacute;&zcaron;dit</trd>
  <trd lng="de">pflastern</trd>
  <trd lng="en">pave</trd>
  <trd lng="es">pavimentar</trd>
  <trdgrp lng="fr">
    <trd>paver<klr> (tout mat&eacute;riaux)</klr></trd>,
    <trd>poser un <ind>rev&ecirc;tement</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kik&ouml;vez</trd>,
    <trd>burkol <klr tip="ind"> (utat)</klr></trd>,
    <trd>flaszteroz</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>plaveien</trd>,
    <trd>bestraten</trd>
  </trdgrp>
  <trd lng="pl">szabrowa&cacute;</trd>
  <trd lng="ru">&c_m;&c_o;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
  <trd lng="ro">r&abreve;pi</trd>
  <trdgrp lng="sk">
    <trd>dl&aacute;&zcaron;di&tcaron;</trd>,
    <trd>vydl&aacute;&zcaron;di&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_r;&c_u;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_m;&c_o;&c_s;&c_t;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="pavim.0ero">
  <kap><tld/>ero</kap>
  <snc mrk="pavim.0ero.KON">
    <dif>
      &Ccirc;iu aparta <ref tip="super" cel="pec.0o.ero">peco</ref> konsistiganta <tld/>on:
      <ekz>
        Pilato sin turnis kaj iris <klr>[&#8230;]</klr> reen sen rigardi ion ajn krom la
        diverskolorajn <tld/>erojn, por ne mispa&scirc;i
        <fnt>
          <bib>MkM</bib>
          <lok>2</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj sandaloj forklakis sur la <tld/>eroj de la korto
        <fnt>
          <bib>MkM</bib>
          <lok>26</lok>
        </fnt>;
      </ekz>
      <ekz>
        teroristoj <klr>[&#8230;]</klr> armitaj &ccirc;ie surloke per bastonoj kaj <tld/>eroj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Monato;/2003/007981.html"
              >Al la intelektuloj de Monato</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">pavez</trd>
  <trd lng="cs">dla&zcaron;dice</trd>
  <trd lng="de">Pflasterstein</trd>
  <trd lng="en">paving stone</trd>
  <trd lng="es">adoqu&iacute;n</trd>
  <trd lng="fr">pav&eacute;</trd>
  <trdgrp lng="hu">
    <trd>burkolatk&odblac;</trd>,
    <trd>flaszterk&odblac;</trd>
  </trdgrp>
  <trd lng="nl">straatsteen</trd>
  <trdgrp lng="pl">
    <trd>brukowiec</trd>,
    <trd>kamie&nacute; brukowy</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>pavaj din piatr&abreve;</trd>,
    <trd>pavaj</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_u;&c_l;&c_y;&c_zh;&c_n;&c_i;&c_k; <klr>(&c_i;&c_z;
  &c_m;&c_o;&c_s;&c_t;&c_o;&c_v;&c_o;&c_j;)</klr></trd>
  <trd lng="sk">dla&zcaron;dica</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_m;&c_ib;&c_n;&c_mol; &c_d;&c_l;&c_ja; &c_b;&c_r;&c_u;&c_k;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja; &c_v;&c_u;&c_l;&c_i;&c_c;&c_ib;</trd>,
    <trd>&c_b;&c_r;&c_u;&c_k;&c_n;&c_ja;&c_k;</trd>,
    <trd>&c_b;&c_u;&c_l;&c_i;&c_zh;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
