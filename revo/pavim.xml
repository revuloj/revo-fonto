<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pavim.xml,v 1.19 2015/07/04 08:37:41 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pavim</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pavim.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pavim.0o.KON">
    <uzo tip="fak">KON</uzo>
    <uzo tip="fak">TRA</uzo>
    <dif>
      Tega&jcirc;o el &scirc;tono, ligno, asfalto kaj simila
      etendita sur tero de strato, vestiblo...
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_u;&c_k;</trd>
  <trd lng="br">pavez<klr tip="ind"> (hollad pavezio&ugrave; ur
  straed)</klr></trd>
  <trd lng="cs">dlažba</trd>
  <trd lng="de">Pflaster</trd>
  <trdgrp lng="en">
    <trd>pavement</trd>,
    <trd>paving</trd>
  </trdgrp>
  <trd lng="es">pavimento</trd>
  <trdgrp lng="fr">
    <trd>rev&ecirc;tement routier</trd>,
    <trd>pavage<klr> (tout mat&eacute;riaux)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&ouml;vezet</trd>,
    <trd>&uacute;tburkolat</trd>
  </trdgrp>
  <trd lng="it">pavimento</trd>
  <trdgrp lng="nl">
    <trd>plaveisel</trd>,
    <trd>bestrating</trd>
  </trdgrp>
  <trd lng="pt">pavimento</trd>
  <trd lng="ru">&c_m;&c_o;&c_s;&c_t;&c_o;&c_v;&c_a;&c_ja;</trd>
  <trd lng="sk">dlažba</trd>
</drv>

<drv mrk="pavim.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pavim.0i.KON">
    <dif>
      Tegi per <tld/>o:
      <ekz>
        en nia vila&gcirc;o ne &ccirc;iuj stratoj estas <tld/>itaj<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        per promesoj estas <tld/>ita la infero
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_u;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="br">paveza&ntilde;</trd>
  <trd lng="cs">dláždit</trd>
  <trd lng="de">pflastern</trd>
  <trd lng="en">pave</trd>
  <trd lng="es">pavimentar</trd>
  <trdgrp lng="fr">
    <trd>paver<klr> (tout mat&eacute;riaux)</klr></trd>,
    <trd>poser un <ind>rev&ecirc;tement</ind></trd>
  </trdgrp><trd lng="fr"></trd>
  <trdgrp lng="hu">
    <trd>kik&ouml;vez</trd>,
    <trd>burkol <klr tip="ind"> (utat)</klr></trd>,
    <trd>flaszteroz</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>plaveien</trd>,
    <trd>bestraten</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_o;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>dláždiť</trd>,
    <trd>vydláždiť</trd>
  </trdgrp>
</drv>

<drv mrk="pavim.0ero">
  <kap><tld/>ero</kap>
  <snc mrk="pavim.0ero.KON">
    <dif>
      &Ccirc;iu aparta peco konsistiganta <tld/>on.
    </dif>
  </snc>
  <trd lng="br">pavez</trd>
  <trd lng="cs">dlaždice</trd>
  <trd lng="de">Pflasterstein</trd>
  <trd lng="en">paving stone</trd>
  <trd lng="es">adoqu&iacute;n</trd>
  <trd lng="fr">pav&eacute;</trd>
  <trdgrp lng="hu">
    <trd>burkolatk&odblac;</trd>,
    <trd>flaszterk&odblac;</trd>
  </trdgrp>
  <trd lng="nl">straatsteen</trd>
  <trd lng="ru">&c_b;&c_u;&c_l;&c_y;&c_zh;&c_n;&c_i;&c_k; <klr>(&c_i;&c_z;
  &c_m;&c_o;&c_s;&c_t;&c_o;&c_v;&c_o;&c_j;)</klr></trd>
  <trd lng="sk">dlaždica</trd>
</drv>
</art>
