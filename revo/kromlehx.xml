<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kromlehx.xml,v 1.3 2022/12/07 10:31:14 revo Exp $">
<kap>
  <rad>kromle&hcirc;</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="kromlehx.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="kromlehx.0o.ARKE">
    <uzo tip="fak">ARKE</uzo>
    <dif>
      <ref tip="dif" cel="cirkl.sxton0o.ARKE">&Scirc;toncirklo<sncref/></ref>:
      <ekz>
        <nom>Stonehenge</nom> estas granda rondforma grupo &dash; <tld/>o &dash;
        de 162 starantaj &scirc;tonegoj
        <fnt>
          <aut>-</aut>
          <vrk><url ref="https://eo.advisor.travel/poi/Stonehenge-1922">
          Stonehenge</url></vrk>
          <lok>Advisor.Travel, [2012]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_o;&c_m;&c_l;&c_je;&c_h;</trd>
  <trd lng="fr">cromlech</trd>
  <trd lng="uk">&#x43A;&c_r;&#x43E;&#x43C;&#x43B;&c_je;&c_h;</trd>
</drv>
</art>

