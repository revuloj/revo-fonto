<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>muĝ</rad>/i
</kap>

<drv mrk="mugx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="mugx.0i.ZOO">
    <dif>
      <ref tip="super" cel="blek.0i.beste">Bleki</ref> kiel bovo.
    </dif>
    <trd lng="be">мычаць</trd>
    <trdgrp lng="en">
      <trd>moo</trd>,
      <trd>low</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>beugler</trd>,
      <trd>meugler</trd>
    </trdgrp>
    <trd lng="nl">loeien</trd>
    <trd lng="ru">мычать</trd>
  </snc>
  <snc mrk="mugx.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="bru.0i.soni">Brui</ref> aŭdigante fortan,
      obtuzan, longe tiratan sonon:
      <ekz>
        <tld/>ado de ŝiphupo;
      </ekz>
      <ekz>
        <tld/>anta popolamaso;
      </ekz>
      <ekz>
        la rivero <tld/>is, ŝveligita de la printempaj pluvoj
        <fnt><bib>FK</bib>, <lok>4, rakontoj, la nigra virino</lok></fnt>;
      </ekz>
      <ekz>
        ventego <klr>...</klr> <tld/>is terure
        <fnt><bib>ElektFab</bib>, <lok>fiŝkaptisto kaj lia
        edzino</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">раўці</trd>
    <trdgrp lng="en">
      <trd>roar</trd>,
      <trd>bellow</trd>
    </trdgrp>
    <trd lng="nl">brullen</trd>
    <trd lng="ru">реветь</trd>
  </snc>
  <trd lng="de">brüllen</trd>
  <trd lng="fr">mugir</trd>
  <trdgrp lng="pt">
    <trd>mugir</trd>,
    <trd>bramir</trd>,
    <trd>bramar</trd>,
    <trd>rugir</trd>
  </trdgrp>
</drv>
</art>
