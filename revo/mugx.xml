<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mugx.xml,v 1.23 2025/04/04 15:12:27 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>mu&gcirc;</rad>/i
  <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="mugx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="mugx.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="bru.0i.soni">Brui</ref> a&ubreve;digante fortan,
      <ref cel="obtuz.0a.MUZ">obtuzan</ref>, longe tiratan sonon:
      <ekz>
        la <tld/>ado de la maro kaj la ondegoj
        <fnt>
          <bib>NT</bib>
          <lok>Luko 21:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        la rivero <tld/>is, &scirc;veligita de la printempaj pluvoj
        <fnt><bib>FK</bib>, <lok>4, rakontoj, la nigra virino</lok></fnt>;
      </ekz>
      <ekz>
        ventego <klr>[&#8230;]</klr> <tld/>is terure
        <fnt><bib>ElektFab</bib>, <lok>fi&scirc;kaptisto kaj lia
        edzino</lok></fnt>.
      </ekz>
      <ekz>
        kiam la korko eliris, en la kolo de la botelo a&ubreve;di&gcirc;is <tld/>o, kaj poste
        kluk-klukado, kiam la vino ver&scirc;i&gcirc;is en la glasojn
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>;
      </ekz>
      <ekz>
        per la krakado de la teleroj kaj pladoj, la <tld/>ado de la patoj, la skui&gcirc;ado
        de la kaseroloj <klr>[&#8230;]</klr> en la kuirejo al ilia tagman&gcirc;o ne trapenetris
        e&ccirc; unu vorteto
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la kaldronoj de la &scirc;ipo kun <tld/>o kaj bruo ellasas vaporon
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        leonoj <tld/>as en la bestejoj
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        estis blekanta bovino en la domo, &gcirc;i <tld/>is stultule, kiel bovinoj faras
        <klr>(vd <ref cel="mu.0">mu</ref>i)</klr>
        <fnt>
          <bib>ChR</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ekkriis en <tld/>ado, kiu pli similis la blekadon de sova&gcirc;a besto ol homan
        vo&ccirc;on
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        de malproksime <tld/>is fora tondro
        <fnt>
          <bib>KKr</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
       <ref cel="hup.0i">hupi</ref>,
       <ref cel="ror.0i">rori</ref>,
       <ref cel="trumpe.0i">trumpeti</ref>
    </refgrp>
    <refgrp tip="vid">
       <ref cel="blek.0i">bleki</ref>,
       <ref cel="rauxk.0a">ra&ubreve;ka</ref>,
       <ref cel="korn.0o.MUZ">korno<sncref/></ref>
    </refgrp>
    <trd lng="be">&c_r;&c_a;&c_w;&c_c;&c_ib;</trd>
    <trdgrp lng="en">
      <trd>roar</trd>,
      <trd>bellow</trd>
    </trdgrp>
    <trd lng="nl">brullen</trd>
    <trd lng="ru">&c_r;&c_je;&c_v;&c_je;&c_t;&c_mol;</trd>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Anka&#365; eblas diri (bove) <ke>bleki</ke>.
  </rim>


<!-- ne speciala, anka&ubreve; leono mu&gcirc;as..., la speciala bovbleko estas mu-i
  <snc mrk="mugx.0i.ZOO">
    <dif>
      <ref tip="super" cel="blek.0i.beste">Bleki</ref> kiel bovo.
    </dif>
    <trd lng="be">&c_m;&c_y;&c_ch;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="en">
      <trd>moo</trd>,
      <trd>low</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>beugler</trd>,
      <trd>meugler</trd>
    </trdgrp>
    <trd lng="nl">loeien</trd>
    <trd lng="pl">rycze&cacute;</trd>
    <trd lng="ru">&c_m;&c_y;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </snc>
-->
  <trdgrp lng="de">
    <trd>br&uuml;llen</trd>,
    <trd>brausen</trd>
  </trdgrp>
  <trd lng="es">mugir</trd>
  <trd lng="fr">mugir</trd>
  <trdgrp lng="ja">
    <trd>&#40180;&#12367;  <pr>&#12394;&#12367;</pr></trd>,
    <trd>&#12358;&#12394;&#12427;</trd>,
    <trd>&#12411;&#12360;&#12427;</trd>,
    <trd>&#24594;&#21495;&#12377;&#12427;  <pr>&#12393;&#12372;&#12358;&#12377;&#12427;</pr></trd>,
    <trd>&#12392;&#12393;&#12429;&#12367;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rycze&cacute;</trd>,
    <trd>wrzeszcze&cacute;</trd>,
    <trd>szumie&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>mugir</trd>,
    <trd>bramir</trd>,
    <trd>bramar</trd>,
    <trd>rugir</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>rage</trd>,
    <trd>zbiera</trd>,
    <trd>mugi</trd>,
    <trd>urla</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_je;&c_v;&c_ib;&c_t;&c_i; (&#1087;&c_r;&#1086; &#1083;&c_je;&c_v;&c_a;</trd>,
    <trd>&c_b;&c_i;&#1082;&c_a;</trd>,
    <trd>&#1087;&c_r;&#1086; &c_b;&c_u;&c_r;&#1102; &c_ib; &c_t;.&#1087;.)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21564; <pr>h&#466;u</pr></trd>,
    <trd>&#21564;&#21483; <pr>h&#466;uji&#224;o</pr></trd>,
    <trd>&#21638; <pr>p&#225;o</pr></trd>
  </trdgrp>
</drv>

</art>
