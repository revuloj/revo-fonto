<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mugx.xml,v 1.21 2024/01/26 19:05:54 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>mu&gcirc;</rad>/i
</kap>

<drv mrk="mugx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="mugx.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="bru.0i.soni">Brui</ref> a&ubreve;digante fortan,
      <ref cel="obtuz.0a.MUZ">obtuzan</ref>, longe tiratan sonon:
      <ekz>
        la <tld/>ado de la maro kaj la ondegoj
        <fnt>
          <bib>NT</bib>
          <lok>Luko 21:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        la rivero <tld/>is, &scirc;veligita de la printempaj pluvoj
        <fnt><bib>FK</bib>, <lok>4, rakontoj, la nigra virino</lok></fnt>;
      </ekz>
      <ekz>      
        ventego <klr>[&#x2026;]</klr> <tld/>is terure
        <fnt><bib>ElektFab</bib>, <lok>fi&scirc;kaptisto kaj lia
        edzino</lok></fnt>.
      </ekz>
      <ekz>
        kiam la korko eliris, en la kolo de la botelo a&ubreve;di&gcirc;is <tld/>o, kaj poste
        kluk-klukado, kiam la vino ver&scirc;i&gcirc;is en la glasojn
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>;
      </ekz>
      <ekz>
        per la krakado de la teleroj kaj pladoj, la <tld/>ado de la patoj, la skui&gcirc;ado
        de la kaseroloj <klr>[&#x2026;]</klr> en la kuirejo al ilia tagman&gcirc;o ne trapenetris
        e&ccirc; unu vorteto
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la kaldronoj de la &scirc;ipo kun <tld/>o kaj bruo ellasas vaporon
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        leonoj <tld/>as en la bestejoj
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        estis blekanta bovino en la domo, &gcirc;i <tld/>is stultule, kiel bovinoj faras
        <klr>(vd <ref cel="mu.0">mu</ref>i)</klr>
        <fnt>
          <bib>ChR</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ekkriis en <tld/>ado, kiu pli similis la blekadon de sova&gcirc;a besto ol homan
        vo&ccirc;on
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        de malproksime <tld/>is fora tondro
        <fnt>
          <bib>KKr</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
       <ref cel="hup.0i">hupi</ref>,
       <ref cel="ror.0i">rori</ref>,
       <ref cel="trumpe.0i">trumpeti</ref>
    </refgrp>
    <refgrp tip="vid">
       <ref cel="blek.0i">bleki</ref>,
       <ref cel="rauxk.0a">ra&ubreve;ka</ref>,
       <ref cel="korn.0o.MUZ">korno<sncref/></ref>
    </refgrp>
    <trd lng="be">&c_r;&c_a;&c_w;&c_c;&c_ib;</trd>
    <trdgrp lng="en">
      <trd>roar</trd>,
      <trd>bellow</trd>
    </trdgrp>
    <trd lng="nl">brullen</trd>
    <trd lng="ru">&c_r;&c_je;&c_v;&c_je;&c_t;&c_mol;</trd>
  </snc>

<!-- ne speciala, anka&ubreve; leono mu&gcirc;as..., la speciala bovbleko estas mu-i
  <snc mrk="mugx.0i.ZOO">
    <dif>
      <ref tip="super" cel="blek.0i.beste">Bleki</ref> kiel bovo.
    </dif>
    <trd lng="be">&c_m;&c_y;&c_ch;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="en">
      <trd>moo</trd>,
      <trd>low</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>beugler</trd>,
      <trd>meugler</trd>
    </trdgrp>
    <trd lng="nl">loeien</trd>
    <trd lng="pl">rycze&cacute;</trd>
    <trd lng="ru">&c_m;&c_y;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </snc>
-->
  <trdgrp lng="de">
    <trd>br&uuml;llen</trd>,
    <trd>brausen</trd>
  </trdgrp>
  <trd lng="es">mugir</trd>
  <trd lng="fr">mugir</trd>
  <trdgrp lng="ja">
    <trd>&#x9CF4;&#x304F;  <pr>&#x306A;&#x304F;</pr></trd>,
    <trd>&#x3046;&#x306A;&#x308B;</trd>,
    <trd>&#x307B;&#x3048;&#x308B;</trd>,
    <trd>&#x6012;&#x53F7;&#x3059;&#x308B;  <pr>&#x3069;&#x3054;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x3068;&#x3069;&#x308D;&#x304F;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rycze&cacute;</trd>,
    <trd>wrzeszcze&cacute;</trd>,
    <trd>szumie&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>mugir</trd>,
    <trd>bramir</trd>,
    <trd>bramar</trd>,
    <trd>rugir</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>rage</trd>,
    <trd>zbiera</trd>,
    <trd>mugi</trd>,
    <trd>urla</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x440;&#x435;&#x432;&#x456;&#x442;&#x438; (&#x43F;&#x440;&#x43E; &#x43B;&#x435;&#x432;&#x430;</trd>,
    <trd>&#x431;&#x438;&#x43A;&#x430;</trd>,
    <trd>&#x43F;&#x440;&#x43E; &#x431;&#x443;&#x440;&#x44E; &#x456; &#x442;.&#x43F;.)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x543C; <pr>h&#x1D2;u</pr></trd>,
    <trd>&#x543C;&#x53EB; <pr>h&#x1D2;uji&#xE0;o</pr></trd>,
    <trd>&#x5486; <pr>p&#xE1;o</pr></trd>
  </trdgrp>
</drv>

</art>
