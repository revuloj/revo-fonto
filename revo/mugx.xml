<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mu&gcirc;</rad>/i
</kap>
<drv mrk="mugx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Bleki kiel bovo.
    </dif>
    <trd lng="nl">loeien</trd>
    <trd lng="ru">&c_m;&c_y;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      A&ubreve;digi fortan, obtuzan, longe tiratan sonon:
      <ekz>
        <tld/>ado de ventego, akvofalo, marondoj, &scirc;iptrumpetego;
      </ekz>
      <ekz>
        <tld/>anta popolamaso.
      </ekz>
    </dif>
    <trd lng="nl">brullen</trd>
    <trd lng="ru">&c_r;&c_je;&c_v;&c_je;&c_t;&c_mol;</trd>
  </snc>
</drv>
</art>
