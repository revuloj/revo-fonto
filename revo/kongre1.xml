<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc> <ofc>1</ofc> <ofc>7</ofc>
  <rad>kongres</rad>/o
</kap>
<drv mrk="kongre1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Solena kunveno de samcelanoj a&ubreve; samprofesianoj, el unu a&ubreve; el diversaj
      landoj, por diskuti komunajn literaturajn, sciencajn, politikajn,
      ekonomiajn demandojn:
      <ekz>
        <ctl>Unua Internacia <tld lit="K"/>o pri
        Ligno</ctl><fnt><aut>Johano
        Pachter</aut>: <vrk>&leftquot;Ligna Hannover&rightquot;: monda foiro pri ligno</vrk>,
        <bib>Monato</bib><lok>jaro 1997a, numero 9a, p. 14a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Kongress</trd>,
    <trd>Tagung</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>conference</trd>,
    <trd>convention</trd>,
  </trdgrp>
  <trdgrp lng="fr">
    <trd>colloque</trd>,
    <trd>congr&egrave;s</trd>
  </trdgrp>
  <trd lng="hu">kongresszus</trd>
  <trd lng="nl">congres</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_malmol;&c_je;&c_z;&c_d;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_g;&c_r;&c_je;&c_s;&c_s;</trd>
  </trdgrp>
</drv>

<drv mrk="kongre1.0i">
  <kap><tld/>i<fnt>Z</fnt></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Kunveni <tld/>e.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>tagen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>convene</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire un <ind>congr&egrave;s</ind></trd>,
    <trd>se r&eacute;unir <klr> (en congr&egrave;s)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kongresszust tart</trd>,
    <trd>kongresszuson vesz r&eacute;szt</trd>
  </trdgrp>
  <trd lng="nl">congresseren</trd>
</drv>

<drv mrk="kongre1.0ano">
  <kap><tld/>ano</kap>
  <snc>
    <dif>
      &Ccirc;eestanto de <tld/>o:
      <ekz>
        pluraj <tld/>anoj a&ccirc;etis tiun libreton<fnt><aut>Wim M. A. De
        Smet</aut>: <vrk>Ekzotaj arboj en Esperantujo (Lilli Giloteaux,
        Arboj en A&ubreve;stralio kaj suda E&ubreve;ropo)</vrk>,
        <bib>Monato</bib><lok>jaro 1997a, numero 10a, p. 19a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>conventioner</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>congressiste</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kongresszusi k&uuml;ld&ouml;tt</trd>,
    <trd>kongresszus r&eacute;sztvev&odblac;je</trd>
  </trdgrp>
</drv>

<drv mrk="kongre1.0ejo">
  <kap><tld/>ejo</kap>
  <uzo tip="fak">ARKI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="konstr.0ajxo">Konstrua&jcirc;o</ref> kun salonegoj,
      ekspoziciejoj ta&ubreve;gaj por <tld/>aj prelegoj, kunsidoj kaj aliaj eroj:
      <ekz>
        en Valencio mi komencis ser&ccirc;i la <tld/>ejon<fnt><aut>Marjorie
        Boulton</aut>: <vrk>Dinomanio</vrk>, <bib>Monato</bib><lok>jaro
        1994a, numero 2a, p. 16a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>conference center</trd>,
    <trd>convention center</trd>,
    <trd>convention hall</trd>,
    <trd>exposition center</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>palais des congr&egrave;s</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kongresszus helysz&iacute;ne</trd>,
    <trd>kongresszusi palota</trd>
  </trdgrp>
</drv>

<drv mrk="kongre1.universala0o">
  <kap>Universala <tld lit="K"/>o</kap>
  <mlg>UK</mlg>
  <uzo tip="fak">ESP</uzo>
  <snc>
    <dif>
      Granda &ccirc;iujara <tld/>o de parolantoj de Esperanto,
      okazanta sinsekve en diversaj urboj de la mondo, okazo por
      priesperantaj, fakaj kaj &gcirc;eneralaj diskutoj kaj iniciatoj:
      <ekz>
        la unua Universala <tld lit="K"/>o okazis en 1905, en
        Bulonjo-&ccirc;e-maro.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>(Esperanto) World <ind>Congress</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Congr&egrave;s universel<klr> (d'esp&eacute;ranto)</klr></trd>
  </trdgrp>
  <trd lng="hu"><klr>(Eszperant&oacute;) </klr>Vil&aacute;gkongresszus</trd>
</drv>
</art>
