  <trd lng="be">&c_m;&c_je;&c_r;&c_a;&c_p;&c_r;&c_y;&c_je;&c_m;&c_s;&c_t;&c_v;&c_y; &c_p;&c_a; <ind>&c_k;&c_a;&c_n;&c_g;&c_r;&c_e;&c_s;&c_je;</ind></trd>
  <trd lng="fr">apr&egrave;s-congr&egrave;s</trd>
  <trd lng="id">pascakongres</trd>
  <trdgrp lng="it">
    <trd><ind>attivit&agrave;</ind> postcongressuali</trd>,
    <trd>incontro <ind>postcongressuale</ind></trd>
  </trdgrp>
  <trd lng="ja">&#x5927;&#x4F1A;&#x5F8C;&#x884C;&#x4E8B;  <pr>&#x305F;&#x3044;&#x304B;&#x3044;&#x3054;&#x304E;&#x3087;&#x3046;&#x3058;</pr></trd>
