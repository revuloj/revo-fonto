<drv mrk="kongre1.antaux0o">
  <kap>anta&ubreve;<tld/>o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Diversaj aran&gcirc;oj, ofte turismaj kaj distraj, anta&ubreve;antaj
      <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_r;&c_a;&c_p;&c_r;&c_y;&c_je;&c_m;&c_s;&c_t;&c_v;&c_y; &c_p;&c_je;&c_r;&c_a;&c_d; <ind>&c_k;&c_a;&c_n;&c_g;&c_r;&c_e;&c_s;&c_a;&c_m;</ind></trd>
  <trd lng="fr">avant-congr&egrave;s</trd>
  <trd lng="id">prakongres</trd>
  <trdgrp lng="it">
    <trd><ind>attivit&agrave;</ind> precongressuali</trd>,
    <trd>incontro <ind>precongressuale</ind></trd>
  </trdgrp>
  <trd lng="nl">voorcongres</trd>
  <trd lng="pl">program przed<ind>kongresowy</ind></trd>
</drv>

<drv mrk="kongre1.post0o">
  <kap>post<tld/>o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
