<drv mrk="kongre1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Solena <ref tip="super" cel="ven.kun0o">kunveno</ref> de samcelanoj a&ubreve; samprofesianoj,
      el unu a&ubreve; el diversaj landoj, por diskuti komunajn
      literaturajn, sciencajn, politikajn, ekonomiajn demandojn:
      <ekz>
        la Bulonja <tld/>o
        <fnt>
          <bib>F</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        Unua Internacia <tld lit="K"/>o pri Ligno
        <fnt><aut>Johano
        Pachter</aut>: <vrk>&leftquot;Ligna Hannover&rightquot;: monda foiro pri
        ligno</vrk>,
        <bib>Monato</bib><lok>jaro 1997a, numero 9a, p. 14a</lok></fnt>;
      </ekz>
      <ekz>
        en la <tld/>o en <nom>Viborg</nom> la Dana nobelaro forbruligas la le&gcirc;ojn de la re&gcirc;o
        <fnt>
          <bib>Fab3</bib>
          <lok>Dorna vojo de la honoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu &ccirc;i Varsovia Duklando, <klr>[&#x2026;]</klr> post la Viena <tld/>o transiris, kiel
        Pola Re&gcirc;olando, sub la rusan sceptron
        <fnt>
          <bib>SrT</bib>
          <lok>Libro I.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">ESP</uzo>
        sciigante la amikojn de l' lingvo internacia pri la intencita
        internacia <tld/>o, mi devas sciigi ilin, ke la tuta sorto de l'
        lingvo internacia de nun transiras en la manojn de l'
        <tld/>o
        <fnt>
          <bib>DL</bib>
          <lok>Dua Libro de l' Lingvo Internacia</lok>
        </fnt>;
      </ekz>
      <ekz>
        supozu ke <klr>[&#x2026;]</klr>, la tuta <tld/>o malfeli&ccirc;e konsistos plene el la plej obstinaj rutinuloj kaj
        malamikoj de &ccirc;io nova
        <fnt>
          <bib>EE</bib>
          <lok>V</lok>
        </fnt>;
      </ekz>
      <ekz>
        la unua e&ubreve;karistia <tld/>o de Litovio okazis en la jaro 1934
        <fnt>
          <bib>Monato</bib>
          <vrk>E&ubreve;karistia kongreso: pre&gcirc;o kaj dancoj</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_n;&c_g;&c_r;&c_e;&c_s;</trd>,
    <trd>&c_z;&c_mol;&c_je;&c_z;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kongres</trd>,
    <trd>shrom&aacute;&zcaron;d&ecaron;n&iacute;</trd>,
    <trd>sjezd</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kongress</trd>,
    <trd>Tagung</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>conference</trd>,
    <trd>convention</trd>,
  </trdgrp>
  <trdgrp lng="es">
    <trd>conferencia</trd>,
    <trd>congreso</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>colloque</trd>,
    <trd>congr&egrave;s</trd>
  </trdgrp>
  <trd lng="fy">kongres</trd>
  <trd lng="hu">kongresszus</trd>
  <trdgrp lng="id">
    <trd>konferensi</trd>,
    <trd>kongres</trd>,
    <trd>muktamat</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>congresso</trd>,
    <trd>conferenza<klr> (convegno)</klr></trd>,
    <trd>convegno<klr> (congresso)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x5927;&#x4F1A; <pr>&#x305F;&#x3044;&#x304B;&#x3044;</pr></trd>
  <trd lng="nl">congres</trd>
  <trdgrp lng="pl">
    <trd>kongres</trd>,
    <trd>konferencja</trd>,
    <trd>zjazd</trd>,
    <trd>zlot</trd>
  </trdgrp>
  <trd lng="pt">congresso</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_malmol;&c_je;&c_z;&c_d;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_g;&c_r;&c_je;&c_s;&c_s;</trd>
  </trdgrp>
  <trd lng="sk">zjazd</trd>
  <trdgrp lng="zh">
    <trd>&#x5927;&#x4F1A; <pr>d&agrave;hu&igrave;</pr></trd>,
    <trd>&#x56FD;&#x4F1A; <pr>gu&oacute;hu&igrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kongre1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="ven.kun0i">Kunveni</ref> <tld/>e:
      <ekz>
        ni <tld/>as nun en glora universitata urbo de Granda Britujo
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Tria Kongreso Esperantista en Cambridge en la 12a de a&ubreve;gusto 1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        SAT <tld/>os en
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 8-9 (58-59)</lok>
        </fnt>;
      </ekz>
      <ekz>
        la unuan fojon esperantistoj de la tuta mondo <tld/>os en Sankta Lando, Israelo
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 3 (65)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_b;&c_r;&c_a;&c_c;&c_c;&c_a; &c_n;&c_a; <ind>&c_k;&c_a;&c_n;&c_g;&c_r;&c_e;&c_s;</ind></trd>
  <trdgrp lng="cs">
    <trd>sn&ecaron;movat</trd>,
    <trd>&uacute;&ccaron;astnit se sjezdu</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>tagen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>convene</trd>
  </trdgrp>
  <trd lng="es">reunirse en un congreso</trd>
  <trdgrp lng="fr">
    <trd>faire un <ind>congr&egrave;s</ind></trd>,
    <trd>se r&eacute;unir <klr> (en congr&egrave;s)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kongresszust tart</trd>,
    <trd>kongresszuson vesz r&eacute;szt</trd>
  </trdgrp>
  <trd lng="id">ber<ind>kongres</ind></trd>
  <trdgrp lng="it">
    <trd>partecipare a un <ind>congresso</ind></trd>,
    <trd>partecipare a una <ind>conferenza</ind></trd>,
    <trd>partecipare a un <ind>convegno</ind></trd>
  </trdgrp>
  <trd lng="ja">&#x5927;&#x4F1A;&#x306B;&#x96C6;&#x307E;&#x308B;
  <pr>&#x305F;&#x3044;&#x304B;&#x3044;&#x306B;&#x3042;&#x3064;&#x307E;&#x308B;</pr></trd>
  <trd lng="nl">congresseren</trd>
  <trd lng="pl">bra&cacute; udzia&lstroke; <klr>(w kongresie, zlocie, konferencji
  ...)</klr></trd>
  <trd lng="ru">&c_s;&c_o;&c_b;&c_r;&c_a;&c_t;&c_mol;&c_s;&c_ja; &c_n;&c_a; <ind>&c_s;&c_malmol;&c_je;&c_z;&c_d;</ind></trd>
  <trd lng="sk">snemova&tcaron;</trd>
</drv>

<drv mrk="kongre1.0ano">
  <kap><tld/>ano</kap>
  <snc>
    <dif>
      &Ccirc;eestanto de <tld/>o:
      <ekz>
        mi permesas al mi en la nomo de &ccirc;iuj <tld/>anoj esprimi nian saluton kaj dankon
        al la lando, kiu gastame nin akceptis
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Tria Kongreso Esperantista en Cambridge en la 12a de a&ubreve;gusto 1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        oni anoncis ke nombro da <tld/>anoj estos oficiale ricevita &ccirc;e la Palaco de la
        Prezidanto
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 5</lok>
        </fnt>;
      </ekz>
      <ekz>
        pluraj <tld/>anoj a&ccirc;etis tiun libreton
        <fnt>
          <aut>Wim M. A. De Smet</aut>:
          <vrk>Ekzotaj arboj en Esperantujo (Lilli Giloteaux,
          Arboj en A&ubreve;stralio kaj suda E&ubreve;ropo)</vrk>,
          <bib>Monato</bib><lok>jaro 1997a, numero 10a, p. 19a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_d;&c_z;&c_je;&c_l;&c_mol;&c_n;&c_ib;&c_k; <ind>&c_k;&c_a;&c_n;&c_g;&c_r;&c_e;&c_s;&c_u;</ind></trd>
  <trd lng="cs">&uacute;&ccaron;astn&iacute;k sjezdu</trd>
  <trdgrp lng="de">
    <trd>Kongressteilnehmer</trd>,
    <trd>Tagungsteilnehmer</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>conventioner</trd>
  </trdgrp>
  <trd lng="es">congresista</trd>
  <trdgrp lng="fr">
    <trd>congressiste</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kongresszusi k&uuml;ld&ouml;tt</trd>,
    <trd>kongresszus r&eacute;sztvev&odblac;je</trd>
  </trdgrp>
  <trd lng="id">pe<ind>serta</ind> kongres</trd>
  <trdgrp lng="it">
    <trd>congressista</trd>,
    <trd>convegnista</trd>
  </trdgrp>
  <trd lng="ja">&#x5927;&#x4F1A;&#x53C2;&#x52A0;&#x8005; <pr>&#x305F;&#x3044;&#x304B;&#x3044;&#x3055;&#x3093;&#x304B;&#x3057;&#x3083;</pr></trd>
  <trdgrp lng="nl">
    <trd>congresganger</trd>,
    <trd>congresdeelnemer</trd>
  </trdgrp>
  <trd lng="pl">uczestnik <klr>(kongresu, zlotu, konferencji
  ...)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_ch;&c_a;&c_s;&c_t;&c_n;&c_i;&c_k; <ind>&c_s;&c_malmol;&c_je;&c_z;&c_d;&c_a;</ind></trd>,
    <trd>&c_u;&c_ch;&c_a;&c_s;&c_t;&c_n;&c_i;&c_k; <ind>&c_k;&c_o;&c_n;&c_g;&c_r;&c_je;&c_s;&c_s;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="sk">&uacute;&ccaron;astn&iacute;k zjazdu</trd>
</drv>

<drv mrk="kongre1.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Loko, en kiu okazas <tld/>o(j):
      <ekz>
        en <tld/>ejon &ccirc;iu nun eniris por la malferma kunsido
        <fnt>
          <bib>VivZam</bib>
          <lok>Vivo de Zamenhof</lok>
        </fnt>;
      </ekz>
      <ekz>
        Barcelono &ndash; urbo elektita kiel <tld/>ejo
        <fnt>
          <bib>DKM</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        en Valencio mi komencis ser&ccirc;i la <tld/>ejon
        <fnt><aut>Marjorie Boulton</aut>:
        <vrk>Dinomanio</vrk>,
        <bib>Monato</bib><lok>jaro
        1994a, numero 2a, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="auxdit.0o">a&ubreve;ditorio</ref>,
      <ref cel="kampus.0o">kampuso</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_a;&c_c; <ind>&c_k;&c_a;&c_n;&c_g;&c_r;&c_e;&c_s;&c_a;&c_w;</ind></trd>
  <trdgrp lng="cs">
    <trd>d&uring;m sjezdu</trd>,
    <trd>m&iacute;stnost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kongresszentrum</trd>,
    <trd>Tagungszentrum</trd>,
    <trd>Tagungsort</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd><ind>conference</ind> center</trd>,
    <trd><ind>convention</ind> center</trd>,
    <trd><ind>convention</ind> hall</trd>,
    <trd><ind>exposition</ind> center</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>palais des congr&egrave;s</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kongresszus helysz&iacute;ne</trd>,
    <trd>kongresszusi palota</trd>
  </trdgrp>
  <trd lng="id"><ind>balai</ind> sidang</trd>
  <trdgrp lng="it">
    <trd>centro congressi</trd>,
    <trd>palazzo del congresso</trd>
  </trdgrp>
  <trd lng="ja">&#x5927;&#x4F1A;&#x4F1A;&#x5834; <pr>&#x305F;&#x3044;&#x304B;&#x3044;&#x304B;&#x3044;&#x3058;&#x3087;&#x3046;</pr></trd>
  <trd lng="nl">congresgebouw</trd>
  <trd lng="pl">centrum <ind>kongresowe</ind></trd>
  <trd lng="ru">&c_d;&c_v;&c_o;&c_r;&c_je;&c_c; <ind>&c_s;&c_malmol;&c_je;&c_z;&c_d;&c_o;&c_v;</ind></trd>
  <trd lng="sk">kongresov&aacute; miestnos&tcaron;</trd>
  <trd lng="zh">&#x4F1A;&#x8BAE;&#x4E2D;&#x5FC3; <pr>hu&igrave;y&igrave;zh&omacron;ngx&imacron;n</pr></trd>
</drv>

<drv mrk="kongre1.universala0o">
  <kap>Universala <tld lit="K"/>o</kap>
  <mlg>UK</mlg>
  <snc>
  <uzo tip="fak">ESP</uzo>
    <dif>
      Granda &ccirc;iujara <tld/>o de parolantoj de Esperanto,
      okazanta sinsekve en diversaj urboj de la mondo, okazo por
      priesperantaj, fakaj kaj &gcirc;eneralaj diskutoj kaj iniciatoj:
      <ekz>
        solena malfermo de Universala <tld lit="K"/>o
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la unua Universala <tld lit="K"/>o okazis en 1905, en
        Bulonjo-&ccirc;e-maro;
      </ekz>
      <ekz>
        la deka Universala <tld lit="K"/>o de l' Esperantistoj, okazonta en Parizo en
        a&ubreve;gusto 1914
        <fnt>
          <bib>VivZam</bib>
          <lok>Vivo de Zamenhof</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_S;&c_u;&c_s;&c_mol;&c_v;&c_je;&c_t;&c_n;&c_y; <ind>&c_k;&c_a;&c_n;&c_g;&c_r;&c_e;&c_s;</ind>
  <klr>(&c_e;&c_s;&c_p;&c_e;&c_r;&c_a;&c_n;&c_t;&c_a;)</klr></trd>
  <trd lng="de">(Esperanto-)<ind>Weltkongress</ind></trd>
  <trdgrp lng="en">
    <trd>(Esperanto) World <ind>Congress</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Congr&egrave;s universel<klr> (d'esp&eacute;ranto)</klr></trd>
  </trdgrp>
  <trd lng="hu"><klr>(Eszperant&oacute;) </klr>Vil&aacute;gkongresszus</trd>
  <trd lng="id"><ind>Kongres</ind> Universal</trd>
  <trdgrp lng="it">
    <trd><ind>congresso</ind> mondiale (d'Esperanto)</trd>
  </trdgrp>
  <trd lng="ja">&#x4E16;&#x754C;&#x30A8;&#x30B9;&#x30DA;&#x30E9;&#x30F3;&#x30C8;&#x5927;&#x4F1A;
  <pr>&#x305B;&#x304B;&#x3044;&#x3048;&#x3059;&#x307A;&#x3089;&#x3093;&#x3068;&#x305F;&#x3044;&#x304B;&#x3044;</pr></trd>
  <trdgrp lng="nl">
    <trd>wereldcongres</trd>,
    <trd>Esperanto-Wereldcongres</trd>
  </trdgrp>
  <trd lng="pl">&Sacute;wiatowy Kongres <klr>Esperanto</klr></trd>
  <trd lng="ru">&c_V;&c_s;&c_je;&c_m;&c_i;&c_r;&c_n;&c_y;&c_j; <ind>&c_s;&c_malmol;&c_je;&c_z;&c_d;</ind>
  &c_e;&c_s;&c_p;&c_je;&c_r;&c_a;&c_n;&c_t;&c_i;&c_s;&c_t;&c_o;&c_v;</trd>
</drv>

<drv mrk="kongre1.internaciajunulara0o">
  <kap>Internacia Junulara <tld lit="K"/>o</kap>
  <mlg>IJK</mlg>
  <snc>
  <uzo tip="fak">ESP</uzo>
    <dif>
      Granda &ccirc;iujara junulara <tld/>o de junaj parolantoj de Esperanto,
      okazanta sinsekve en diversaj urboj de la mondo:
      <ekz>
        la 55a Internacia Junulara <tld lit="K"/>o en Zanko, apud Balatono
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 3 (53)</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu dokumento <klr>[kiun]</klr> oni debatis, okaze de la 36a Internacia Junulara
        <tld lit="K"/>o <klr>[&#x2026;]</klr> ankora&ubreve; ne esprimas la oficialan opinion de
        TEJO kiel organizo
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 8-9</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_S;&c_u;&c_s;&c_mol;&c_v;&c_je;&c_t;&c_n;&c_y; &c_ju;&c_n;&c_a;&c_c;&c_k;&c_ib; <ind>&c_k;&c_a;&c_n;&c_g;&c_r;&c_e;&c_s;</ind> <klr>(&c_e;&c_s;&c_p;&c_e;&c_r;&c_a;&c_n;&c_t;&c_a;)</klr></trd>
  <trd lng="en">International Youth <ind>Congress</ind> (of
  Esperanto)</trd>
  <trd lng="fy">Ynternasjonaal Jongereinkongres</trd>
  <trd lng="ja">&#x56FD;&#x969B;&#x9752;&#x5E74;&#x30A8;&#x30B9;&#x30DA;&#x30E9;&#x30F3;&#x30C8;&#x5927;&#x4F1A;
  <pr>&#x3053;&#x304F;&#x3055;&#x3044;&#x305B;&#x3044;&#x306D;&#x3093;&#x3048;&#x3059;&#x307A;&#x3089;&#x3093;&#x3068;&#x305F;&#x3044;&#x304B;&#x3044;</pr></trd>
  <trd lng="id"><ind>Kongres</ind> Pemuda Internasional</trd>
  <trd lng="nl">Internationaal Jongerencongres</trd>
  <trd lng="pl">&Sacute;wiatowy Kongres <klr>Esperanto</klr> M&lstroke;odzie&zdot;y</trd>
</drv>

<drv mrk="kongre1.antaux0o">
  <kap>anta&ubreve;<tld/>o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Diversaj aran&gcirc;oj, ofte turismaj kaj distraj, anta&ubreve;antaj
      <tld/>on:
      <ekz>
        junulara anta&ubreve;<tld/>o en Peterburgo
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 12 (110)</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve;<tld/>o Koro de malnova Ruslando okazos en <nom>Vladimir</nom>
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 12 (62)</lok>
        </fnt>;
      </ekz>
      <ekz>
        Anta&ubreve;<tld/>a Seminario, dedi&ccirc;ita al tri temoj: E-instruado, E-historio kaj &leftquot;Kiel
        krei efikan klubon&rightquot;
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#x2116;3</lok>
        </fnt>.
      </ekz>
