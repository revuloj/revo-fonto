<art mrk="$Id: jul.xml,v 1.13 2024/04/11 07:24:05 revo Exp $">
<kap>
  <rad>Jul</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="jul.0o">
  <kap><tld/>o</kap>
  <snc mrk="jul.0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Mezvintra <ref tip="lst" cel="fest.0o" lst="voko:festoj">festo</ref>
      en Norde&ubreve;ropaj landoj:
      <ekz>
        <tld/>o <klr>...</klr> estas en la skandinaviaj lingvoj la kaj
        kristana kaj nekristana vorto por la festo, kiun oni celebras
        balda&ubreve; post la kaprikorna solstico
        <fnt>
          <aut>M. Kampanja&hcirc;a</aut>,
          <vrk>&Hcirc;aoso</vrk>,
          <lok>
<url ref="&SCE;">
          soc.culture.esperanto</url>, 1995-03-29</lok>
        </fnt>;
      </ekz>
      <ekz>
        feli&ccirc;an <tld/>on!
        <fnt>
          <aut>noktema</aut>,
          <vrk>
<url ref="http://noktema.livejournal.com/21210.html">[blogo]</url></vrk>,
          <lok>2006-12-22</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_jo;&c_l;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>Jezul&aacute;tko</trd>,
    <trd>Je&zcaron;&iacute;&scaron;ek</trd>
  </trdgrp>
  <trd lng="de">Julfest</trd>
  <trd lng="fr">Yule</trd>
  <trdgrp lng="it">
    <trd>Yule</trd>,
    <trd>festa del solstizio d'inverno</trd>
  </trdgrp>
  <trd lng="ja">&#x30E6;&#x30FC;&#x30EB;&#x796D;  <pr>&#x30E6;&#x30FC;&#x30EB;&#12373;&#12356;</pr></trd>
  <trd lng="nl">yule</trd>
  <trd lng="sk">Jezuliatko</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_v;&#x44F;&c_t;&#x43E; &c_z;&c_i;&#x43C;&#x43E;&c_v;&#x43E;&c_g;&#x43E;
    &c_s;&#x43E;&#x43D;&c_c;&c_je;&c_s;&c_t;&#x43E;&#x44F;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_s;&c_v;&#x44F;&c_t;&#x43A;&c_i;</trd>
  </trdgrp>
</drv>
</art>

