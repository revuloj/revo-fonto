<art mrk="$Id: komiks.xml,v 1.27 2025/04/01 05:07:38 revo Exp $">
<kap>
<rad>komiks</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="komiks.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TIP</uzo>
  <snc mrk="komiks.0o.bildrakonto">
    <dif>
      <ref tip="dif" cel="rakont.bild0o">Bildrakonto</ref>:
      <ekz>
        la teknika aran&gcirc;o de <klr>[&#8230;]</klr> japanaj <tld/>oj, estas
        malfacila
        <fnt>
        <aut>Walter Klag</aut>:
        <vrk><url ref="&Monato;/2002/007736.html">Japana komikso (Tezuka
        Osamu, La flambirdo)</url></vrk>,
        <bib>Monato</bib>
        <lok>2002/08, p. 20</lok></fnt>.
      </ekz>
      <ekz>
        la &leftquot;aventuro&#8221; estis nur duonpa&gcirc;a kelkbilda <tld/>o, sed &gcirc;i estis por mi la
        kulmino de la revuo
        <fnt>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="&Monato;/2005/008749.php">Nostalgia reveno</url></vrk>
          <bib>Monato</bib>
          <lok>2005/11, p. 25</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur unu a&ubreve; sur du pa&gcirc;oj, prezenti&gcirc;as unu kompleta <tld/>a bildserio
        <fnt>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2004/008429.html">Komiksoj bonintencaj, sed tre
          mallertaj</url></vrk>
          <bib>Monato</bib>
          <lok>2004/11, p. 26</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>bildrakonto</ke>, <ke>bild&#349;erco</ke>.
  </rim>

  <trd lng="be">&c_k;&c_o;&c_m;&c_ib;&c_k;&c_s;</trd>
  <trd lng="ca">c&ograve;mic</trd>
  <trdgrp lng="cs">
    <trd>komiks</trd>,
    <trd>kreslen&yacute; p&rcaron;&iacute;b&ecaron;h</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Comic</trd>,
    <trd>Bildergeschichte</trd>,
    <trd>Heftchen <klr>(Comic)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>comic</trd>,
    <trd>comic book</trd>,
    <trd>comic strip</trd>,
    <trd>graphic novel</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>c&oacute;mic</trd>,
    <trd>tebeo</trd>
  </trdgrp>
  <trd lng="fa">&a_d;&a_A;&a_s;&a_t;&a_A;&a_n; &a_m;&a_S;&a_w;&a_r;</trd>
  <trdgrp lng="fr">
    <trd>bande dessin&eacute;e</trd>,
    <trd>BD</trd>
  </trdgrp>
  <trd lng="hu">k&eacute;preg&eacute;ny</trd>
  <trdgrp lng="ja">
    <trd>&#12467;&#12511;&#12483;&#12463;&#12473;</trd>,
    <trd>&#21127;&#30011;  <pr>&#12370;&#12365;&#12364;</pr></trd>,
    <trd>&#28459;&#30011;  <pr>&#12414;&#12435;&#12364;</pr></trd>
  </trdgrp>
  <trd lng="nl">strip</trd>
  <trd lng="pl">komiks</trd>
  <trdgrp lng="pt">
    <trd>hist&oacute;ria em quadrinhos</trd>,
    <trd>HQ</trd>,
    <trd>banda desenhada</trd>
  </trdgrp>
  <trd lng="ro">benzi desenate</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_i;&c_k;&c_s;</trd>
  <trdgrp lng="sk">
    <trd>komiks</trd>,
    <trd>kreslen&yacute; pr&iacute;beh</trd>
  </trdgrp>
  <trd lng="uk">&c_k;&c_o;&c_m;&c_ib;&c_k;&c_s;</trd>
  <trdgrp lng="zh">
    <trd>&#36830;&#29615;&#22270; <pr>li&#225;nhu&#225;nt&#250;</pr></trd>,
    <trd>&#36830;&#29615;&#30011; <pr>li&#225;nhu&#225;nhu&#224;</pr></trd>
  </trdgrp>
</drv>
