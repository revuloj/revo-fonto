<drv mrk="indik.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        Johano rigardis la&ubreve; &scirc;ia <tld/>o kaj restis silenta
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>T-Online</nom> <klr>[estas]</klr> la&ubreve; propra <tld/>o, plej granda provizanto en
        Germanio
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2004/008342.html">Bela komputila mondo</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="montr">montro</ref>,
      <ref cel="recept.0o">recepto</ref>
    </refgrp>
    <ref tip="malprt" cel="instru1.0o.KOMUNE">instrukcio</ref>
  </snc>
  <snc mrk="indik.0o.sciigo">
    <dif>
      Unuopa <ref cel="sci.0igo">sciigo</ref>, kiun oni donas <tld/>ante;
      <ref tip="dif" cel="indik.0ajxo"><tld/>a&jcirc;o</ref>:
      <ekz>
        precizaj <tld/>oj pri la nomoj, datoj, titoloj de diversaj verkoj
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        trovi iun en Romo ne estis facile, e&ccirc; kun plej bonaj <tld/>oj
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o pri traduko el la germana lingvo
        <fnt>
          <bib>Fab4</bib>
          <lok>Infano en la tombo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="datum.0o.SCI">datumo<sncref/></ref>,
      <ref cel="fakt.0o">fakto</ref>
    </refgrp>
  </snc>
  <snc mrk="indik.0o.simptomo">
    <dif>
      Tio, kio <tld/>as<sncref ref="indik.0i.supozigi"/>:
      <ekz>
        li ne sukcesis tamen trovi e&ccirc; plej etan postsignon a&ubreve; <tld/>on
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        el multaj <tld/>oj disigitaj en la poemo oni povas dedukti
        e&ccirc; la daton
        <fnt>
        <aut>Donald Broadribb</aut>
        <vrk><url ref="&Monato;/2002/pdf/monato200205.pdf#page=27">La Purgatorio vidita de
        dektriajarcentulo</url></vrk>.
          <bib>Monato</bib>
          <lok>2002/05, p. 27-29</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="simpto.0o">simptomo</ref>
    </refgrp>
    <trd lng="de">Indiz</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_k;&c_a;&c_z;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_v;&c_y;&c_z;&c_n;&c_a;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ozna&ccaron;en&iacute;</trd>,
    <trd>&uacute;daj</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hinweis</trd>,
    <trd>Anzeige</trd>,
    <trd>Angabe</trd>
  </trdgrp>
  <trd lng="es">indicaci&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>indication</trd>,
    <trd>indice</trd>,
    <trd>conseil</trd>,
    <trd>recommandation</trd>,
    <trd>signe</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>jelz&eacute;s</trd>,
    <trd>megad&aacute;s</trd>,
    <trd>javallat</trd>,
    <trd>adat</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8868;&#x793A;  [<ind>&#x3072;&#x3087;&#x3046;&#x3058;</ind>]</trd>,
    <trd>&#x6307;&#x793A;  [<ind>&#x3057;&#x3058;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>aanwijzing</trd>,
    <trd>aanduiding</trd>
  </trdgrp>
  <trd lng="pt">indica&ccedil;&atilde;o<klr tip="amb"> (ato de
      indicar)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_k;&c_a;&c_z;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_z;&c_n;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ozna&ccaron;enie</trd>,
    <trd>&uacute;daj</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_k;&c_a;&c_z;&c_ib;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_z;&c_n;&c_a;&c_ch;&c_je;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="indik.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      <ref cel="datum.0o.SCI">Datumo</ref>,
      fakto, signo, fenomeno, kiu gvidas al io, kiu supozigas
      a&ubreve; helpas trovi ion; duonpruvo, duonmontro:
      <ekz>
        &ccirc;u la izraelidoj iam toleris aliajn diojn, <klr>[jen]</klr> la arkeologiaj
        <tld/>a&jcirc;oj ne estas certaj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=347">I&scirc;tar</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a&jcirc;oj montras pli kompleksan rilaton inter salo kaj korvaskula malsano
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=8786">Salo</url></lok>
        </fnt>.
      </ekz>
      <trdgrp lng="de">
        <trd>Indiz</trd>,
        <trd>Verdachtsgrund</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>adat</trd>,
        <trd>jelz&eacute;s</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>aanwijzing</trd>,
        <trd>indicatie</trd>
      </trdgrp>
    </dif>
    <subsnc mrk="indik.0ajxo.JUR">
      <dif>
        &Ccirc;iu el la cirkonstancoj, el kiuj oni deduktas pruvon por
        konvikto (kontraste al konfeso a&ubreve; rekta atesto):
        <ekz>
          la&ubreve; la mezepokaj konceptoj, gravis por la ju&gcirc;o a&ubreve; kredindaj vid-atestantoj a&ubreve;
          konfeso, kaj ne la <tld/>a&jcirc;oj
          <fnt>
            <bib>Viki</bib>
            <lok><url ref="https://eo.wikipedia.org/?curid=25663">Torturo</url></lok>
          </fnt>;
        </ekz>
        <ekz>
          la pruvo per la <ind><tld/>a&jcirc;o</ind>j estas la plej
          malforta
          <fnt><bib>PIV1</bib></fnt>.
          <trd lng="en">circumstantial evidence</trd>
          <trdgrp lng="hu">
            <trd>gyan&uacute;ok</trd>,
            <trd>indicium</trd>
          </trdgrp>
          <trd lng="ru">&c_k;&c_o;&c_s;&c_v;&c_je;&c_n;&c_n;&c_a;&c_ja; &c_u;&c_l;&c_i;&c_k;&c_a;</trd>
        </ekz>
      </dif>
    </subsnc>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_k;&c_a;&c_z;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_ib;&c_n;&c_d;&c_e;&c_k;&c_s;</trd>,
    <trd>&c_d;&c_o;&c_k;&c_a;&c_z;</trd>
