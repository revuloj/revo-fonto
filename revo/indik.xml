    <trd>&c_ib;&c_n;&c_d;&c_i;&c_k;&c_a;&c_t;&c_o;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6307;&#x793A;&#x7B26; <pr>zh&#x1D0;sh&igrave;f&uacute;</pr></trd>,
    <trd>&#x6307;&#x793A;&#x8005; <pr>zh&#x1D0;sh&igrave;zh&ecaron;</pr></trd>,
    <trd>&#x8DE1;&#x8C61; <pr>j&igrave;xi&agrave;ng</pr></trd>,
    <trd>&#x516C;&#x793A; <pr>g&omacron;ngsh&igrave;</pr></trd>,
    <trd>&#x6307;&#x793A;&#x7269; <pr>zh&#x1D0;sh&igrave;w&ugrave;</pr></trd>,
    <trd>&#x7DDA;&#x7D22; <pr>xi&agrave;nsu&#x1D2;</pr></trd>,
    <trd>&#x4F5C;&#x6807;&#x8BB0; <pr>zu&ograve;bi&amacron;oj&igrave;</pr></trd>,
    <trd>&#x6838;&#x5BF9;&#x6807;&#x8BB0; <pr>h&eacute;du&igrave;bi&amacron;oj&igrave;</pr></trd>,
    <trd>&#x8A18;&#x865F; <pr>j&igrave;h&agrave;o</pr></trd>,
