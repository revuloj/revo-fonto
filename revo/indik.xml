<drv mrk="indik.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      <ref cel="datum.0o.SCI">Datumo</ref>,
      fakto, signo, fenomeno, kiu gvidas al io, kiu supozigas
      a&ubreve; helpas trovi ion; duonpruvo, duonmontro:
      <ekz>
        &ccirc;u la izraelidoj iam toleris aliajn diojn, <klr>[jen]</klr> la arkeologiaj
        <tld/>a&jcirc;oj ne estas certaj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=347">I&scirc;tar</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a&jcirc;oj montras pli kompleksan rilaton inter salo kaj korvaskula malsano
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=8786">Salo</url></lok>
        </fnt>.
      </ekz>
      <trdgrp lng="de">
        <trd>Indiz</trd>,
        <trd>Verdachtsgrund</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>adat</trd>,
        <trd>jelz&eacute;s</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>aanwijzing</trd>,
        <trd>indicatie</trd>
      </trdgrp>
    </dif>
    <subsnc mrk="indik.0ajxo.JUR">
      <dif>
        &Ccirc;iu el la cirkonstancoj, el kiuj oni deduktas pruvon por
        konvikto (kontraste al konfeso a&ubreve; rekta atesto):
        <ekz>
          la&ubreve; la mezepokaj konceptoj, gravis por la ju&gcirc;o a&ubreve; kredindaj vid-atestantoj a&ubreve;
          konfeso, kaj ne la <tld/>a&jcirc;oj
          <fnt>
            <bib>Viki</bib>
            <lok><url ref="https://eo.wikipedia.org/?curid=25663">Torturo</url></lok>
          </fnt>;
        </ekz>
        <ekz>
          la pruvo per la <ind><tld/>a&jcirc;o</ind>j estas la plej
          malforta
          <fnt><bib>PIV1</bib></fnt>.
          <trd lng="en">circumstantial evidence</trd>
          <trdgrp lng="hu">
            <trd>gyan&uacute;ok</trd>,
            <trd>indicium</trd>
          </trdgrp>
          <trd lng="ru">&c_k;&c_o;&c_s;&c_v;&c_je;&c_n;&c_n;&c_a;&c_ja; &c_u;&c_l;&c_i;&c_k;&c_a;</trd>
        </ekz>
      </dif>
    </subsnc>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_k;&c_a;&c_z;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_ib;&c_n;&c_d;&c_e;&c_k;&c_s;</trd>,
    <trd>&c_d;&c_o;&c_k;&c_a;&c_z;</trd>
  </trdgrp>
  <trd lng="en">evidence</trd>
  <trd lng="fr">indice</trd>
  <trdgrp lng="ja">
    <trd>&#x5FB4;&#x5019;  <pr>&#x3061;&#x3087;&#x3046;&#x3053;&#x3046;</pr></trd>,
    <trd>&#x624B;&#x304C;&#x304B;&#x308A;  <pr>&#x3066;&#x304C;&#x304B;&#x308A;</pr></trd>,
    <trd>&#x6C17;&#x914D;  <pr>&#x3051;&#x306F;&#x3044;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>poszlaka</trd>,
    <trd>wyznacznik</trd>
  </trdgrp>
  <trd lng="pt">indica&ccedil;&atilde;o<klr tip="amb"> (coisa)</klr></trd>
  <trd lng="ru">&c_u;&c_k;&c_a;&c_z;&c_a;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="uk">
    <trd>&c_o;&c_z;&c_n;&c_a;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_a;&c_z;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_ib;&c_n;&c_d;&c_je;&c_k;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4E8B;&#x8B49; <pr>sh&igrave;zh&egrave;ng</pr></trd>,
    <trd>&#x4F50;&#x8B49; <pr>zu&#x1D2;zh&egrave;ng</pr></trd>,
