<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
<ofc>1</ofc>
      <rad>task</rad>/o
    </kap>
    <drv mrk="task.0o">
      <kap><tld/>o</kap>
      <snc mrk="task.0o.ordonita">
        <dif>
          Ordonita laboro, servo farenda en difinita tempo:
          <ekz>
            mi &jcirc;us finis mian <tld/>on;
          </ekz><ekz>
            la lernantoj faras siajn <tld/>ojn.
          </ekz>
        </dif>
        <trd lng="he">&mem;&tet;&lamed;&he;</trd>
        <trdgrp lng="nl"> 
           <trd>taak</trd>,
           <trd>opgave</trd>,
           <trd>huiswerk</trd>,
           <trd>opdracht</trd>
        </trdgrp>
        <trdgrp lng="pt">
           <trd>tarefa</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_z;&c_a;&c_d;&c_a;&c_n;&c_i;&c_je;</trd>,
          <trd>&c_z;&c_a;&c_d;&c_a;&c_ch;&c_a;</trd>,
          <trd>&c_u;&c_r;&c_o;&c_k;</trd></trdgrp>
        <trd lng="sv">uppgift</trd>
      </snc>
      <snc mrk="task.0o.entrepreno">
        <dif>
          Difinita laboro a&ubreve; servo:
          <ekz>
            preni sur sin la malfacilan <tld/>on aran&gcirc;i la kongreson<fnt>Z</fnt>;
          </ekz><ekz>
            mi starigis (proponis, metis) al mi la <tld/>on... fari;
          </ekz><ekz>
            jen la bela <tld/>o, kiu levi&gcirc;as anta&ubreve; ni.
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="entrep.0o">entrepreno</ref>,
          <ref cel="rol.0o">rolo</ref>,
          <ref cel="ofic.0o">ofico</ref>,
          <ref cel="dejxor.0o">de&jcirc;oro</ref>,
          <ref cel="okup.0o">okupo</ref>,
          <ref cel="profes.0o">profesio</ref>,
          <ref cel="meti.0o">metio</ref>.
        </refgrp>
        <trdgrp lng="nl"> 
           <trd>taak</trd>,
           <trd>opdracht</trd>
        </trdgrp>
        <trd lng="ru">&c_z;&c_a;&c_d;&c_a;&c_ch;&c_a;</trd>
        <trd lng="sv">uppgift</trd>
      </snc>
      <snc mrk="task.0o.KOMP">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          <ref tip="dif" cel="procez.0o.KOMP">procezo</ref>.
        </dif>
        <trd lng="ru">&c_z;&c_a;&c_d;&c_a;&c_ch;&c_a;</trd>
      </snc>
      <trdgrp lng="be">
        <trd>&c_z;&c_a;&c_d;&c_a;&c_ch;&c_a;</trd>,
        <trd>&c_z;&c_a;&c_d;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
      </trdgrp>
        <trdgrp lng="nl"> 
           <trd>taak</trd>,
           <trd>proces</trd>
        </trdgrp>
      <trd lng="cs">&uacute;kol</trd>
      <trdgrp lng="de">
          <trd>Aufgabe</trd>,
          <trd>Auftrag</trd>
      </trdgrp>
      <trd lng="en">task</trd>
      <trd lng="he">&mem;&shin;&yod;&mem;&he;</trd>
      <trd lng="hu">feladat</trd>
      <trd lng="sv">process</trd>
      <trd lng="vo">sugiv</trd>
    </drv>
    <drv mrk="task.0i">
      <kap><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Difini, trudi al iu kiel <tld/>on:
          <ekz>
            ili ne povis diveni la aferojn, kiujn &scirc;i <tld/>is al ili<fnt>Z</fnt>.
          </ekz>  
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_d;&c_a;&c_r;&c_u;&c_ch;&c_y;&c_c;&c_mol; &c_z;&c_a;&c_d;&c_a;&c_ch;&c_u;</trd>,
        <trd>&c_s;&c_t;&c_a;&c_v;&c_ib;&c_c;&c_mol; &c_z;&c_a;&c_d;&c_a;&c_ch;&c_u;</trd>
      </trdgrp>
      <trd lng="he">&lamed;&he;&tet;&yod;&lamed; \<ind>&nun;&tet;&lamed;</ind>\<klr tip="ind"> (&mem;&shin;&yod;&mem;&he;)</klr></trd>
      <trdgrp lng="hu">
        <trd>megb&iacute;z <klr tip="ind">(feladattal)</klr></trd>,
        <trd>felad <klr tip="ind">(pl. leck&eacute;t)</klr></trd>,
        <trd>kiad <klr tip="ind">(feladatot)</klr></trd>
      </trdgrp>
      <trd lng="nl">opdragen</trd>
      <trd lng="ru">&c_p;&c_o;&c_r;&c_u;&c_ch;&c_i;&c_t;&c_mol; <klr>(&c_z;&c_a;&c_d;&c_a;&c_ch;&c_u;)</klr></trd>
    </drv>

    <drv mrk="task.plur0ado">
      <kap>plur<tld/>ado</kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          <ref tip="dif" cel="rul.kun0o">kunrulo</ref>.
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_sh;&c_m;&c_a;&c_t;&c_z;&c_a;&c_d;&c_a;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
        <trd>&c_m;&c_u;&c_l;&c_mol;&c_t;&c_y;&c_z;&c_a;&c_d;&c_a;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
      </trdgrp>
      <trd lng="en">multitasking</trd>
      <trd lng="he">&resh;&yod;&bet;&vav;&yod; &mem;&shin;&yod;&mem;&vav;&tav;</trd>
      <trdgrp lng="hu">
        <trd>t&ouml;bbfeladatos feldolgoz&aacute;s</trd>,
        <trd>p&aacute;rhuzamos feldolgoz&aacute;s</trd>,
        <trd>multitasking</trd>
      </trdgrp>
      <trd lng="ru">&c_m;&c_n;&c_o;&c_g;&c_o;&c_z;&c_a;&c_d;&c_a;&c_ch;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
    </drv>
  </art>
