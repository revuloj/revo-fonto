<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>papag</rad>/o
</kap>
<drv mrk="papag.0o">
<kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="papag.0o.komune">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Iu el <tld/>edoj
    </dif>
  </snc>
  <snc mrk="papag.0o.zoo">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="papage.0oj"><tld/>edoj</ref>
      (<trd lng="la">Psittacus</trd>), tropika grimpobirdo, 
      kapabla imiti la homan vo&ccirc;on kaj &ccirc;iuspecajn kriojn.
    </dif>
  </snc>
  <snc mrk="papag.0o.ripetulo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Homo, kiu senkomprene ripetas instruon:
      <ekz>
        <tld/>e ripeti gurditan lecionon;
      </ekz>
      <ekz>
        lernejo ne devas esti <tld/>ejo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Papagei</trd>
  <trd lng="fr">perroquet</trd>
  <trd lng="tr">papa&gbreve;an</trd>
</drv>

<drv mrk="papag.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Psittaciformes</trd>).
      Grimpantaj, tropikaj birdoj, kun brilaj diverskoloraj plumoj
      kaj hoka beko.
    </dif>
  </snc>
  <trd lng="de">Papageienv&ouml;gel</trd>
</drv>
</art>
