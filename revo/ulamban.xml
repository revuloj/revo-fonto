<kap>
  <rad>Ulamban</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="ulamban.0o">
     <kap><tld/>o</kap>
     <snc mrk="ulamban.0o.BUD">
       <uzo tip="fak">BUD</uzo>
       <uzo tip="fak">KAL</uzo>
       <dif>
         Budaisma <ref tip="lst" cel="fest.0o" lst="voko:festoj">festo</ref>
          por mortintaj prauloj:
         <ekz>
           Sansa Danco estas unu el dancadoj en <tld lit="u"/>o
           <fnt>
             <aut>Sasaki M.</aut>,
             <vrk><url ref="http://morioka.cool.ne.jp/ihatov-esperanto/sansa.html">Kio
             estas Sansa Danco?</url></vrk>,
             <lok>2004-08-09</lok>
           </fnt>;
         </ekz>
         <ekz>
           <tld/>a akrido <klr>...</klr> aperas ofte
           &ccirc;irka&ubreve; la tagoj de <tld lit="u"/>o (t.e. en a&ubreve;gusto)
           <fnt>
           <aut>Vastalto</aut>,
           <vrk><url ref="http://www.vastalto.com/blog">Akvosfero</url></vrk>,
           <lok>2006-08-03</lok>
           </fnt>.
         </ekz>
       </dif>
     </snc>
  <trd lng="cs">japonsk&yacute; sv&aacute;tek mrtv&yacute;ch</trd>
  <trdgrp lng="de">
    <trd>Ullambana</trd>
    <trd>Allerseelenfest</trd>
  </trdgrp>
  <trdgrp lng="fi">
         <trd>ullambana</trd>,
         <trd><klr tip="amb">(buddhalainen) </klr> esi-isien
         juhla</trd>,
         <trd>obon-juhla <klr> (Japanissa)</klr></trd>
     </trdgrp>
  <trd lng="fr">Ullambana<klr> (f&ecirc;te des morts bouddhique)</klr></trd>
  <trdgrp lng="it">
         <trd>ullambana</trd>,
         <trd>y&uacute;l&aacute;np&eacute;n<klr> (cina)</klr></trd>,
         <trd>obon<klr> (giappone)</klr></trd>
     </trdgrp>
  <trd lng="pl">nabo&zdot;e&nacute;stwo japo&nacute;skie</trd>
  <trd lng="sk">japonsk&yacute; sviatok m&racute;tvych</trd>
  <trdgrp lng="zh">
    <trd>&#x4E07;&#x7075;&#x8282; <pr>w&#xE0;nl&#xED;ngji&#xE9;</pr></trd>,
    <trd>&#x842C;&#x9748;&#x7BC0; <pr>w&#xE0;nl&#xED;ngji&#xE9;</pr></trd>
  </trdgrp>
</drv>
</art>

