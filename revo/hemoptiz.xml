<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hemoptiz.xml,v 1.10 2024/02/02 09:23:27 revo Exp $">
<kap>
  <rad>hemoptiz</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="hemoptiz.0o">
  <kap><tld/>o</kap>
  <snc mrk="hemoptiz.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Sangelsputo pro sangado en la pulmoj:
      <ekz>
        &scirc;i resani&gcirc;is el la <tld/>o, tamen post laci&gcirc;o <klr>...</klr>
        &scirc;i
        ofte tusis
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 77a, volumo 2a, p.
        627a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_v;&c_a;&c_h;&c_a;&c_r;&c_k;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="cs">hemoptysis</trd>
  <trdgrp lng="de">
    <trd>H&auml;moptyse</trd>,
    <trd>Bluthusten</trd>
  </trdgrp>
  <trd lng="fr">h&eacute;moptysie</trd>
  <trd lng="it">emottisi</trd>
  <trdgrp lng="la">
    <trd>haemopto&euml;</trd>,
    <trd>haemoptysis</trd>
  </trdgrp>
  <trd lng="pl">krwioplucie</trd>
  <trd lng="pt">hemoptise</trd>
  <trd lng="sk">chrlenie krvi</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_je;&#x43C;&#x43E;&#x43F;&c_t;&c_i;&c_z;&c_ib;&#x44F;</trd>,
    <trd>&#x43A;&c_r;&#x43E;&c_v;&#x43E;&c_h;&c_a;&c_r;&#x43A;&c_a;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
  <trd lng="zh">&#x54AF;&#34880; <pr>lu&#xF2;xu&#xE8;</pr></trd>
</drv>
</art>

