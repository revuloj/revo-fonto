<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: varingjen.xml,v 1.15 2023/09/05 09:43:54 revo Exp $">
    <kap>
      <rad>Varingjen</rad>/o <fnt><bib>PIV2</bib></fnt>
    </kap>

    <drv mrk="varingjen.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">ESP</uzo>
      <uzo tip="fak">PERS</uzo>
      <snc>
        <dif>
          Franca esperanta vortaristo kaj beletristo (1901&ndash;1991),
          &ccirc;efredaktisto de Plena Ilustrita Vortaro (PIV),
          tradukinto de diversaj verkoj, originala eseisto kaj poemisto
          foje uzinta ka&scirc;nomojn:
          <ekz>
            Poeto! Viglu via laring'! Jen |
            Nia persist'
            <klr>[pers/ist'; aludo al la Kajam-tradukoj]</klr>
            persista. <tld/>', |
            ree bonfaras al la verda gento
            <fnt>
              <aut>G&eacute;rald C. JERVIS</aut>:
              <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr13/suplemento.html">Saluto al Suplemento kompilita de
                  Prof. Waringhien</url>
              </vrk>.
              <lok>La nica literatura revuo. N-ro&nbsp;3 (1956, 1:3) p.
108</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <rim>
        La&ubreve; la ekzemplo en PIV2 atribuita tie al <tld/>o,
        &scirc;ajnas ke li
        mem preferis &gcirc;uste &ccirc;i tian transskribon:
        <ekz>
          vi longe skribis al mi, amiko <tld/>';
        </ekz>
        en la literaturo, cetere, troveblas anka&ubreve; pli fonetika
        transskribo:
        <ekz>
          la dorsa kovrilo de Eseoj III de Varengjeno
          <fnt>
            <aut>Jorge Camacho</aut>:
            <vrk>La Majstro kaj Martinelli</vrk>.
            <lok>Budape&scirc;to, 1993</lok>
          </fnt>.          
        </ekz>
      </rim>
      <trd lng="ca">Waringhien</trd>
      <trd lng="es">Waringhien</trd>
      <trd lng="fr">Gaston <ind>Waringhien</ind></trd>
      <trd lng="de">Gaston <ind>Waringhien</ind></trd>
      <trd lng="hu">Waringhien</trd>
      <trd lng="it">Waringhien</trd>
      <trd lng="ja">&#x30F4;&#x30A1;&#x30E9;&#x30F3;&#x30AE;&#x30E3;&#x30F3;</trd>
  <trd lng="nl">Gaston <ind>Waringhien</ind></trd>
      <trd lng="pt">Waringhien</trd>
      <trd lng="ru">&c_G;&c_a;&c_s;&c_t;&c_o;&c_n;
        <ind>&c_V;&c_a;&c_r;&c_je;&c_n;&c_g;&c_mol;&c_je;&c_n;</ind></trd>
      <trd lng="sk">G.Waringhien</trd>
  <trd lng="cs">G.Waringhien</trd>
  <trd lng="uk">&#x412;&#x430;&#x440;&#x435;&#x43D;&#x491;&#x2019;&#x454;&#x43D; (&#x435;&#x441;&#x43F;&#x435;&#x440;&#x430;&#x43D;&#x442;&#x43E;&#x43C;&#x43E;&#x432;&#x43D;&#x438;&#x439; &#x43F;&#x438;&#x441;&#x44C;&#x43C;&#x435;&#x43D;&#x43D;&#x438;&#x43A;)</trd>
</drv>

    <drv mrk="varingjen.0a">
      <kap><tld lit="v"/>a</kap>
      <snc>
        <dif>
          Rilata al <tld/>o, al liaj verkaro, stilo:
          <ekz>
            la nevalidecon de la <tld lit="v"/>a rezono pruvas la fakto,
            ke oni povas returni la argumenton
            <fnt><bib>BonaLingvo</bib>, <lok>p. 81a</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="ca">waringhieni&agrave;</trd>
      <trdgrp lng="hu">
        <trd>waringhieni</trd>,
        <trd>Waringhien-f&eacute;le</trd>
      </trdgrp>
      <trdgrp lng="it">
        <trd>waringhiano</trd>,
        <trd>di <ind>Waringhien</ind></trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>op de manier van <ind>Waringhien</ind></trd>,
        <trd>van <ind>Waringhien</ind></trd>
      </trdgrp>
      <trd lng="ru">&c_V;&c_a;&c_r;&c_je;&c_n;&c_g;&c_mol;&c_je;&c_n;&c_o;&c_v;</trd>
    </drv>
  </art>

