<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: advers.xml,v 1.9 2025/03/24 20:41:55 revo Exp $">
<kap>
  <rad>advers</rad>/a <fnt><bib>SPIV</bib></fnt>
</kap>
<drv mrk="advers.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="advers.0a.kontrauxa">
    <dif>
      <ref tip="super" cel="kontraux.0a.KOMUNE">Kontra&ubreve;a</ref>,
      malamika en batalo, konkuro, proceso...:
      <ekz>
        <tld/>a atako, <tld/>a gambito
        <fnt>
          <aut>Remu&scirc;</aut>,
          <vrk><url ref="http://remush.be/tezauro/vortoj/%C5%9Dakx.html">
          &Scirc;ako</url></vrk>,
          <lok>[vidita en 2017]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>kontra&#365;a</ke>, <ke>kontra&#365;aga</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_p;&c_r;&c_a;&c_c;&c_ib;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_c;&c_ib;&c_l;&c_je;&c_g;&c_l;&c_y;</trd>,
    <trd>&c_a;&c_n;&c_t;&c_a;&c_g;&c_a;&c_n;&c_ib;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">adverzativn&iacute;</trd>
  <trdgrp lng="fr">
    <trd>adverse</trd>,
    <trd>adversaire<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="he">&#1513;&#1473;&#1456;&#1500;&#1460;&#1497;&#1500;&#1460;&#1497;</trd>
  <trdgrp lng="pl">
    <trd>niesprzyjaj&aogonek;cy</trd>,
    <trd>nieprzychylny</trd>
  </trdgrp>
  <trd lng="ro">defavorabil</trd>
  <trd lng="sk">odporovac&iacute;</trd>
  <trdgrp lng="uk">
    <trd>&#1087;&c_r;&#1086;&c_t;&c_i;&c_v;&#1085;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_u;&#1087;&c_r;&#1086;&c_t;&c_i;&c_v;&#1085;&c_i;&c_j;</trd>,
    <trd>&c_v;&#1086;&c_r;&#1086;&c_zh;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="advers.0ulo">
  <kap><tld/>ulo</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="advers.0ulo.KOMUNE">
    <dif>
      <ref tip="super" cel="kontraux.0ulo.KOMUNE">Kontra&ubreve;ulo</ref>,
      kontra&ubreve;ludanto, kontra&ubreve;luktanto:
      <ekz>
        iu staras kun glavo kontra&ubreve; l' <tld/>ulo nuda
        <fnt>
          <aut>I. Mad&aacute;ch, trad. K. Kalocsay</aut>, 
          <vrk>La Tragedio de l' Homo</vrk>, 
          <lok>1965</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>kontra&#365;ulo</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_p;&c_r;&c_a;&c_c;&c_ib;&c_w;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_a;&c_n;&c_t;&c_a;&c_g;&c_a;&c_n;&c_ib;&c_s;&c_t;</trd>
  </trdgrp>
  <trd lng="fr">adversaire<klr> (subst.)</klr></trd>
  <trd lng="he">&#1497;&#1464;&#1512;&#1460;&#1497;&#1489;</trd>
  <trdgrp lng="pl">
    <trd>adwersarz</trd>,
    <trd>przeciwnik <klr>(ideowy)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#1087;&c_r;&#1086;&c_t;&c_i;&c_v;&#1085;&c_i;&#1082;</trd>,
    <trd>&c_s;&c_u;&#1087;&c_r;&#1086;&c_t;&c_i;&c_v;&#1085;&c_i;&#1082;</trd>,
    <trd>&c_v;&#1086;&c_r;&#1086;&c_g;</trd>,
    <trd>&c_s;&c_u;&#1087;&c_je;&c_r;&#1085;&c_i;&#1082;</trd>
  </trdgrp>
</drv>
</art>

