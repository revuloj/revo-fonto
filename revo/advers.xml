<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: advers.xml,v 1.5 2017/11/26 15:10:20 revo Exp $">
<kap>
  <rad>advers</rad>/a <fnt><bib>SPIV</bib></fnt>
</kap>
<drv mrk="advers.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="advers.0a.kontrauxa">
    <dif>
      <ref tip="super" cel="kontraux.0a.KOMUNE">Kontra&ubreve;a</ref>,
      malamika en batalo, konkuro, proceso...:
      <ekz>
        <tld/>a atako, <tld/>a gambito
        <fnt>
          <aut>Remu&scirc;</aut>,
          <vrk><url
          ref="http://remush.be/tezauro/vortoj/%C5%9Dakx.html">
          &Scirc;ako</url></vrk>,
          <lok>[vidita en 2017]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">adverzativn&iacute;</trd>
  <trdgrp lng="fr">
    <trd>adverse</trd>,
    <trd>adversaire<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>niesprzyjaj&aogonek;cy</trd>,
    <trd>nieprzychylny</trd>
  </trdgrp>
  <trd lng="ro">defavorabil</trd>
  <trd lng="sk">odporovac&iacute;</trd>
</drv>

<drv mrk="advers.0ulo">
  <kap><tld/>ulo</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="advers.0ulo.KOMUNE">
    <dif>
      <ref tip="super" cel="kontraux.0ulo.KOMUNE">Kontra&ubreve;ulo</ref>,
      kontra&ubreve;ludanto, kontra&ubreve;luktanto:
      <ekz>
        iu staras kun glavo kontra&ubreve; l' <tld/>ulo nuda
        <fnt>
          <aut>I. Mad&aacute;ch, trad. K. Kalocsay</aut>, 
          <vrk>La Tragedio de l' Homo</vrk>, 
          <lok>1965</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">adversaire<klr> (subst.)</klr></trd>
  <trdgrp lng="pl">
    <trd>adwersarz</trd>,
    <trd>przeciwnik <klr>(ideowy)</klr></trd>
  </trdgrp>
</drv>
</art>

