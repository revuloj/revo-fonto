<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>suk</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="suk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="suk.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Organika nutra flua&jcirc;o:
      <ekz>
        <tld/>o de viando;
      </ekz>
      <ekz>
        citron<tld/>o.
      </ekz>
    </dif>
    <trd lng="bg">&c_s;&c_o;&c_k;</trd>
    <trd lng="ca">suc</trd>
    <trd lng="de">Saft</trd>
    <trd lng="en">juice</trd>
    <trd lng="es">zumo</trd>
    <trdgrp lng="hu">
      <trd>l&eacute;</trd>,
      <trd>nedv</trd>
    </trdgrp>
    <trd lng="nl">sap</trd>
  </snc>
  <snc mrk="suk.0o.plejgravajxo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kerna esenco, plejgrava&jcirc;o:
      <ekz>
        tiu libreto enhavus la <tld/>on de la doktrino.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="medol.0o">medolo</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Wesen</trd>,
      <trd>Essenz</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>l&eacute;nyeg</trd>,
      <trd>esszencia</trd>,
      <trd>veleje</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>essentie</trd>,
      <trd>wezen<klr tip="ind"> (essentie)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_s;&c_o;&c_k;</trd>
  <trd lng="bg">&c_s;&c_o;&c_k;</trd>
  <trd lng="de">Saft</trd>
  <trd lng="en">juice</trd>
  <trdgrp lng="fr">
    <trd>jus</trd>,
    <trd>suc</trd>
  </trdgrp>
  <trd lng="tp">telo</trd>
  <trd lng="ru">&c_s;&c_o;&c_k;</trd>
</drv>

<drv mrk="suk.0a">
  <kap><tld/>a</kap>
  <snc mrk="suk.0a.el_suko">
    <dif>
      El <tld/>o.
    </dif>
    <trdgrp lng="hu">
      <trd>l&eacute;-</trd>,
      <trd>leves-</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_o;&c_k;&c_o;&c_v;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="suk.0a.sukoplena">
    <dif>
      <tld lit="S"/>oplena:
      <ekz>
        <tld/>e verda folio<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_a;&c_k;&c_a;&c_v;&c_ib;&c_t;&c_y;</trd>
    <trd lng="bg">&c_s;&c_o;&c_ch;&c_je;&c_n;</trd>
    <trd lng="de">saftig</trd>
    <trdgrp lng="hu">
      <trd>l&eacute;d&uacute;s</trd>,
      <trd>leveses</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trd lng="bg">&c_s;&c_o;&c_ch;&c_je;&c_n;</trd>
  <trd lng="de">saftig</trd>
  <trd lng="en">juicy</trd>
  <trd lng="fr">juteux</trd>
  <trd lng="nl">sappig</trd>
</drv>

<drv mrk="suk.flor0o">
  <kap>flor<tld/>o</kap>
  <snc mrk="suk.flor0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <ref tip="dif" cel="nektar.0o.BOT">nektaro <sncref/></ref>
    <ekz>
      la Muzoj estas flirtemaj abeloj; <klr>...</klr> kaptante nur la
      flor<tld/>on de io, balda&ubreve; ili rapidas al nova
      <fnt><aut>J. B. Gresset, trad. C. Rousseau</aut>,
      <vrk>Ver-vert</vrk>, <lok>Esperantista Centra Librejo,
[1914?]</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="bg">&c_n;&c_je;&c_k;&c_t;&c_a;&c_r;</trd>
  <trd lng="de">Nektar</trd>
  <trd lng="fr">nectar<klr> (botanique)</klr></trd>
</drv>

<drv mrk="suk.sen0a">
  <kap>sen<tld/>a</kap>
  <snc>
    <dif>
      Tute senforta:
      <ekz>
        sen<tld/>a <klr>(senhuma)</klr> tero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        sen<tld/>i&gcirc;is la vinbero<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sek.0igxi">seki&gcirc;i</ref>,
      <ref cel="velk.0i">velki</ref>
    </refgrp>
  </snc>
  <trd lng="fr">d&eacute;ss&eacute;ch&eacute;</trd>
  <trdgrp lng="hu">
    <trd>sz&aacute;raz</trd>,
    <trd>v&eacute;rszeg&eacute;ny</trd>,
    <trd>er&odblac;tlen</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>dor</trd>,
    <trd>droog</trd>
  </trdgrp>
</drv>

<drv mrk="suk.0ricxa">
  <kap><tld/>ri&ccirc;a</kap>
  <snc>
    <dif>
      Plena je abunda <tld/>o:
      <ekz>
        &ccirc;ie li trovis <tld/>plenan verda&jcirc;on<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_k;&c_a;&c_v;&c_ib;&c_t;&c_y;</trd>
  <trdgrp lng="hu">
    <trd>l&eacute;d&uacute;s</trd>,
    <trd>leveses</trd>
  </trdgrp>
  <trd lng="fr">juteux</trd>
  <trd lng="nl">sappig</trd>
  <trd lng="ru">&c_s;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
