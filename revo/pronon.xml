<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>prononc</rad>/i
</kap>
<drv mrk="pronon.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="dif" cel="parol.el0i">Elparoli</ref>.
      <ekz>
        en Esperanto oni skribas la vortojn tiel same, kiel oni ilin
        <tld/>as;
      </ekz>
      <ekz>
        insiste <tld/>i
        (<refgrp tip="sin">
          <ref cel="martel.0i">marteli</ref>,
          <ref cel="akcent.0i">akcenti</ref>
        </refgrp>);
      </ekz>
      <ekz>
        malfacile <tld/>ebla nomo;
      </ekz>
      <ekz>
        malregula, stranga <ind><tld/>o</ind>.
        <trd lng="ru">&c_p;&c_r;&c_o;&c_i;&c_z;&c_n;&c_o;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">vyslovit</trd>
  <trd lng="de">aussprechen</trd>
  <trd lng="en">pronounce</trd>
  <trd lng="fr">prononcer</trd>
  <trd lng="nl">uitspreken</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_i;&c_z;&c_n;&c_o;&c_s;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sv">uttala</trd>
</drv>
</art>
