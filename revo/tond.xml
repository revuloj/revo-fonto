<kap>
  <ofc>*</ofc>
  <rad>tond</rad>/i
</kap>

<drv mrk="tond.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      For<ref tip="super" cel="trancx.0i.KOMUNE">tran&ccirc;i</ref> el io per <tld/>ilo:
      <ekz>
        <tld/>i siajn &scirc;afojn
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 31:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni <tld/>os miajn harojn
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 16:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        la konsekrito <ind>pri<tld/>os</ind> <klr>(vd <ref cel="tonsur.0o">tonsuro</ref>)</klr>
        <klr>[&#8230;]</klr> sian konsekritan kapon
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 6:18</lok>
        </fnt>;
        <trd lng="de">beschneiden</trd>
      </ekz>
      <ekz>
        vi jam <tld/>is al mi la barbon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino kaj Rozulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        via patro <tld/>as la hundojn de l' vila&gcirc;o
        <fnt>
          <bib>LSB</bib>
          <lok>Averto</lok>
        </fnt>;
      </ekz>
      <ekz>
        nia &gcirc;ardeno estis grandeta, ni <tld/>is per senmotora ilo nur parton de
        <klr>[&#8230;]</klr> la herboj
        <fnt><aut>Jouko Lindstedt</aut>,
        <vrk><url ref="&Monato;/2000/006918.php">La helikoj</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/07, p. 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        se iu nin &ccirc;ikanas, ni penas lin <tld/>i
        <klr>(ignori, forrifuzi)</klr>, kaj ni tute lin ne indulgas
        <fnt>
          <bib>GDn</bib>
          <lok>Akto Dua</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="raz.0i">razi</ref>,
      <ref cel="falcx.0i">fal&ccirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_r;&c_y;&c_g;&c_ch;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>ost&rcaron;&iacute;hat</trd>,
    <trd>st&rcaron;&iacute;hat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>scheren</trd>,
    <trd><klr>(mit der Schere)</klr> schneiden</trd>
  </trdgrp>
  <trd lng="en">shear</trd>
  <trd lng="es">cortar con tijera</trd>
  <trdgrp lng="fr">
    <trd>couper<klr tip="ind"> (aux ciseaux)</klr></trd>,
    <trd>d&eacute;couper<klr tip="ind"> (aux ciseaux)</klr></trd>,
    <trd>tondre<klr tip="ind"> (aux ciseaux)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ny&iacute;r</trd>,
    <trd>v&aacute;g<klr tip="ind"> (oll&oacute;val)</klr></trd>,
    <trd>nyes<klr tip="ind"> (oll&oacute;val)</klr></trd>
  </trdgrp>
  <trd lng="id">meng<ind>gunting</ind></trd>
  <trdgrp lng="ja">
    <trd>&#21000;&#x308B;  <pr>&#x304B;&#x308B;</pr></trd>,
    <trd>&#21000;&#x308A;&#x8FBC;&#12416;  <pr>&#x304B;&#x308A;&#12371;&#12416;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>knippen</trd>,
    <trd>scheren</trd>,
    <trd>snoeien</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>strzyc<klr> (w&lstroke;osy)</klr></trd>,
    <trd>ci&aogonek;&cacute;<klr> (no&zdot;ycami)</klr></trd>,
    <trd>kraja&cacute;<klr> (no&zdot;ycami)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_i;&c_ch;&c_mol;</trd>
  <trd lng="sk">striha&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_r;&c_i;&c_g;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_s;&c_t;&c_r;&c_i;&c_g;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_t;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_t;&c_n;&c_u;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_b;&c_r;&c_ib;&c_z;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x526A; <pr>ji&#x1CE;n</pr></trd>
</drv>

<drv mrk="tond.0ilo">
  <kap><ofc>*</ofc><tld/>ilo
  <fnt><bib>F</bib>, <lok>Universala Vortaro, -il'</lok></fnt></kap>
  <snc>
    <dif>
      Ilo el du interkruci&gcirc;antaj almoveblaj
      klingoj, kies interna rando estas tran&ccirc;a:
      <ekz>
        per tondilo ni tondas
        <fnt><bib>F</bib> <lok>&FE; 34</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>ilo ne tu&scirc;is mian kapon, &ccirc;ar mi estas konsekrita al Dio
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 16:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        la knabinoj prenis <tld/>ilon kaj fortran&ccirc;is la finon de la barbo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino kaj Rozulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>vito<tld/>ilo</ind> speciale farita tiucele
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Unu</lok>
        </fnt>;
        <trd lng="de">Rebschere</trd>
      </ekz>
      <ekz>
        tablon frapas tajloro, tuj <tld/>ilo eksonas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tenajl.0o">tenajlo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_zh;&c_n;&c_ib;&c_c;&c_y;</trd>
  <trd lng="cs">n&uring;&zcaron;ky</trd>
  <trd lng="de">Schere</trd>
  <trdgrp lng="en">
    <trd>shears</trd>
    <trd>scissors</trd>
  </trdgrp>
  <trd lng="es">tijera</trd>
  <trd lng="fr">ciseaux</trd>
  <trd lng="hu">oll&oacute;</trd>
  <trd lng="id">gunting</trd>
  <trdgrp lng="ja">
    <trd>&#x306F;&#12373;&#x307F;</trd>,
    <trd>&#x30D0;&#x30EA;&#x30AB;&#x30F3;</trd>
  </trdgrp>
  <trd lng="nl">schaar</trd>
  <trdgrp lng="pl">
    <trd>no&zdot;yce</trd>,
    <trd>no&zdot;yczki</trd>
  </trdgrp>
  <trd lng="ro">forfecu&#x21B;&abreve;</trd>
  <trd lng="ru">&c_n;&c_o;&c_zh;&c_n;&c_i;&c_c;&c_y;</trd>
  <trd lng="sk">no&zcaron;nice</trd>
  <trdgrp lng="uk">
    <trd>&c_n;&c_o;&c_zh;&c_i;&c_c;&c_ib;</trd>,
    <trd>&c_m;&c_a;&c_sh;&c_i;&c_n;&c_k;&c_a; &c_d;&c_l;&c_ja; &c_s;&c_t;&c_r;&c_i;&c_zh;&c_k;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x526A;&#20992; <pr>ji&#x1CE;nd&amacron;o</pr></trd>,
    <trd>&#x63C3; <pr>ji&amacron;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="tond.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="T"/>i &ccirc;iuflanke:
      <ekz>
        ne &ccirc;irka&ubreve;<tld/>u la flankojn de via kapo
        kaj ne fordifektu la flankojn de via barbo
        <fnt><bib>MT</bib><lok>&Lev; 19:27</lok></fnt>;
      </ekz>
      <ekz>
        hundeto, &ccirc;irka&ubreve;<tld/>ita tiel mallonge, ke oni
        povis vidi &gcirc;ian rozokoloran ha&ubreve;ton
        <fnt><bib>Fab1</bib><lok>Kupra porko</lok></fnt>;
      </ekz>
      <ekz>
        ondforme &ccirc;irka&ubreve;<tld/>itaj azaleoj kun ru&gcirc;aj floroj
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Yasuo</aut>
          <vrk><url ref="&Monato;/2007/009114.php">Vizito al Takaha&scirc;i</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_s;&c_t;&c_r;&c_y;&c_g;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_r;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_c;&c_ib;&c_n;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">beschneiden</trd>
  <trd lng="fr">tondre</trd>
  <trdgrp lng="hu">
    <trd>k&ouml;rbev&aacute;g <klr tip="ind">(oll&oacute;val)</klr></trd>,
    <trd>k&ouml;rbeny&iacute;r</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ostrzyc</trd>,
    <trd>obcina&cacute;<klr> dooko&lstroke;a</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_o;&c_b;&c_s;&c_t;&c_r;&c_i;&c_g;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_b;&c_t;&c_i;&c_n;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_b;&c_r;&c_ib;&c_z;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#21011; <pr>w&#xE1;n</pr></trd>
</drv>

<drv mrk="tond.de0i">
  <kap>de<tld/>i, <var><kap>for<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Forigi <tld/>ante, preni <tld/>ante:
      <ekz>
        de<tld/>u viajn harojn kaj for&jcirc;etu ilin
        <fnt><bib>MT</bib><lok>&Jer; 7:29</lok></fnt>;
      </ekz>
      <ekz>
        Trebinto lasas for<tld/>i sian belan barbon
        <fnt>
          <bib>KrB</bib>
          <lok>&Ccirc;u socialismo konstrui&gcirc;as?</lok>
        </fnt>;
      </ekz>
      <ekz>
        la maljunulo de<tld/>is <klr>[la foton]</klr> de la kovrilo
        kaj enkadrigis &gcirc;in
        <fnt>
          <aut>Wang Jianbing</aut>
          <vrk>El Popola &Ccirc;inio</vrk>
          <lok><url ref="http://www.china.org.cn/EL/EL/ElPopolaCxinio/bd98-8-5.html">Mia
          maljuna amiko Liu Bingjun</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        ili kreskigas siajn harojn, tiam for<tld/>as ilin por vendi
        <fnt>
          <bib>StU</bib>
          <lok>Al Romo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_s;&c_t;&c_r;&c_y;&c_g;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_r;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_c;&c_ib;&c_n;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>odst&rcaron;ihnout</trd>,
    <trd>ust&rcaron;ihnout</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&ocirc;ter<klr tip="ind"> (en coupant)</klr></trd>,
    <trd>enlever<klr tip="ind"> (en coupant)</klr></trd>,
    <trd>d&eacute;couper<klr tip="ind"> (prendre en coupant avec des
    ciseaux)</klr></trd>,
    <trd>tailler<klr tip="ind"> (enlever en coupant)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>leny&iacute;r</trd>,
    <trd>lev&aacute;g <klr tip="ind">(oll&oacute;val)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#20999;&#x308A;&#x843D;&#12392;&#12377;  <pr>&#x304D;&#x308A;&#x304A;&#12392;&#12377;</pr></trd>
  <trdgrp lng="pl">
    <trd>wycina&cacute;</trd>,
    <trd>odcina&cacute;</trd>,
    <trd>odkrawa&cacute;</trd>
  </trdgrp>
  <trd lng="sk">odstrihn&uacute;&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_t;&c_i;&c_n;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_r;&c_ib;&c_z;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x65A9;&#x65AD; <pr>zh&#x1CE;ndu&#xE0;n</pr></trd>,
    <trd>&#x65AC;&#x65B7; <pr>zh&#x1CE;ndu&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="tond.gazon0ilo">
  <kap>
    gazon<tld/>ilo,
    <var><kap>razen<tld/>ilo</kap></var>
  </kap>
  <snc mrk="tond.gazon0ilo.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      Ma&scirc;ino por <tld/>i gazonon, herbejon:
      <ekz>
        unu el la radoj elfalis el la gazon<tld/>ilo kiam mi <tld/>is la gazonon
        <fnt>
          <aut>Pri Io Ajn</aut>
          <vrk><url
          ref="https://priioajn.wordpress.com/2019/05/22/riparinte-la-gazontondilon/">Riparinte la
          gazontondilon</url></vrk>
          <lok>2019</lok>
        </fnt>;
      </ekz>
      <ekz>
        la dinamo ekfunkcias kiel sputanta malnova razen<tld/>ilo, diras <nom>Sweet</nom>
        <fnt>
          <aut>C. Bowles, trad. D. Harlow</aut>
          <vrk><url
          ref="https://listserv.brown.edu/archives/cgi-bin/wa?A2=auxlang;614d24e7.0108E">&Ccirc;u la tera
          magneta kampo paneas?</url></vrk>
          <lok>2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_z;&c_o;&c_n;&c_a;&c_k;&c_a;&c_s;&c_ib;&c_l;&c_k;&c_a;</trd>
  <trd lng="bg">&c_g;&c_r;&c_a;&c_d;&c_i;&c_n;&c_s;&c_k;&c_a; &c_k;&c_o;&c_s;&c_a;&c_ch;&c_k;&c_a;</trd>
  <trd lng="de">Rasenm&auml;her</trd>
  <trd lng="fr"><ind>tondeuse</ind> &agrave; gazon</trd>
  <trd lng="pl">kosiarka<klr> (do trawy)</klr></trd>
  <trdgrp lng="zh">
    <trd>&#33609;&#x576A;&#x673A; <pr>c&#x1CE;op&#xED;ngj&#x12B;</pr></trd>,
    <trd>&#33609;&#x576A;&#x6A5F; <pr>c&#x1CE;op&#xED;ngj&#x12B;</pr></trd>,
    <trd>&#33609;&#x576A;&#21106;&#33609;&#x673A; <pr>c&#x1CE;op&#xED;ngg&emacron;c&#x1CE;oj&#x12B;</pr></trd>,
    <trd>&#33609;&#x576A;&#21106;&#33609;&#x6A5F; <pr>c&#x1CE;op&#xED;ngg&emacron;c&#x1CE;oj&#x12B;</pr></trd>,
    <trd>&#21000;&#33609;&#x673A; <pr>y&#xEC;c&#x1CE;oj&#x12B;</pr></trd>,
    <trd>&#21000;&#33609;&#x6A5F; <pr>y&#xEC;c&#x1CE;oj&#x12B;</pr></trd>,
    <trd>&#x526A;&#33609;&#x673A; <pr>ji&#x1CE;nc&#x1CE;oj&#x12B;</pr></trd>,
    <trd>&#x526A;&#33609;&#x6A5F; <pr>ji&#x1CE;nc&#x1CE;oj&#x12B;</pr></trd>,
    <trd>&#21106;&#33609;&#x673A; <pr>g&emacron;c&#x1CE;oj&#x12B;</pr></trd>,
    <trd>&#21106;&#33609;&#x6A5F; <pr>g&emacron;c&#x1CE;oj&#x12B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="tond.gxarden0ilo">
  <kap>
    &gcirc;arden<tld/>ilo,
    <var><kap>&gcirc;ardena <tld/>ilo</kap></var>
  </kap>
  <snc mrk="tond.gxarden0ilo.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      Fortika <tld/>ilo, per kiu oni detran&ccirc;as bran&ccirc;etojn kaj similajn, ordinare kun
      mallongaj iom kurbaj klingoj:
      <ekz>
        li enmanigis al mi &gcirc;arden<tld/>ilon kaj igis min <tld/>i la&ubreve;ron
        <fnt>
          <aut>B. Bre&hcirc;to, trad. V. Lutermano</aut>
          <vrk><url ref="http://vlutermano.free.fr/kojno.html">Historioj pri sinjoro
          Kojno</url></vrk>
          <lok>[vidita en 2021]</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi tran&ccirc;is la&ubreve;longe duonige la botelon per &gcirc;ardena <tld/>ilo
        <fnt>
          <aut>E. Rolland</aut>
          <vrk><url ref="http://gxardeneto.chez-alice.fr/ex/akcepto.html">&Gcirc;ardenisto en la
          Lernejo</url></vrk>
          <lok>[vidita en 2021]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_k;&c_a;&c_t;&c_a;&c_r;</trd>
  <trd lng="de">Gartenschere</trd>
  <trd lng="fr">s&eacute;cateur</trd>
  <trd lng="pl">no&zdot;yce ogrodowe</trd>
</drv>
</art>
