<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tond</rad>/i
</kap>
<drv mrk="tond.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fortran&ccirc;i el io per <tld/>ilo:
      <ekz>
        <tld/>i la harojn, la barbon al iu;
      </ekz>
      <ekz>
        <tld/>i &scirc;afon, arbon;
      </ekz>
      <ekz>
        mallonge &gcirc;isnude <tld/>ita hundo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="raz.0i">razi</ref>,
      <ref cel="falcx.0i">fal&ccirc;i</ref>,
      <ref cel="tonsur.0o">tonsuro</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_r;&c_y;&c_g;&c_ch;&c_y;</trd>
  <trd lng="de">scheren</trd>
  <trd lng="en">shear</trd>
  <trdgrp lng="fr">
    <trd>couper<klr tip="ind"> (aux ciseaux)</klr></trd>,
    <trd>d&eacute;couper<klr tip="ind"> (aux ciseaux)</klr></trd>,
    <trd>tondre<klr tip="ind"> (aux ciseaux)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ny&iacute;r</trd>,
    <trd>v&aacute;g<klr tip="ind"> (oll&oacute;val)</klr></trd>,
    <trd>nyes<klr tip="ind"> (oll&oacute;val)</klr></trd>
  </trdgrp>  
  <trdgrp lng="nl">
    <trd>knippen</trd>,
    <trd>scheren</trd>,
    <trd>snoeien</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_i;&c_ch;&c_mol;</trd>
  <trd lng="sk">striha&tcaron;</trd>
</drv>

<drv mrk="tond.0ilo">
  <kap><ofc>*</ofc><tld/>ilo</kap>
  <snc>
    <dif>
      Instrumento, konsistanta el du interkruci&gcirc;antaj almoveblaj
      klingoj, kies interna rando estas tran&ccirc;a.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_zh;&c_n;&c_ib;&c_c;&c_y;</trd>
  <trd lng="de">Schere</trd>
  <trd lng="fr">ciseaux</trd>
  <trd lng="hu">oll&oacute;</trd>  
  <trd lng="nl">schaar</trd>
  <trd lng="ru">&c_n;&c_o;&c_zh;&c_n;&c_i;&c_c;&c_y;</trd>
  <trd lng="sk">no&zcaron;nice</trd>
</drv>

<drv mrk="tond.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="T"/>i &ccirc;iuflanke:
      <ekz>
        ne &ccirc;irka&ubreve;<tld/>u la flankojn de via kapo
        kaj ne fordifektu la flankojn de via barbo
        <fnt><bib>MT</bib><lok>&Lev; 19:27</lok></fnt>;
      </ekz>
      <ekz>
        hundeto, &ccirc;irka&ubreve;<tld/>ita tiel mallonge, ke oni
        povis vidi &gcirc;ian rozokoloran ha&ubreve;ton
        <fnt><bib>Fab1</bib><lok>Kupra porko</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">tondre</trd>
  <trdgrp lng="hu">
    <trd>k&ouml;rbev&aacute;g <klr tip="ind">(oll&oacute;val)</klr></trd>,
    <trd>k&ouml;rbeny&iacute;r</trd>
  </trdgrp>
</drv>

<drv mrk="tond.de0i">
  <kap>de<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Forigi <tld/>ante, preni <tld/>ante:
      <ekz>
        de<tld/>u viajn harojn kaj for&jcirc;etu ilin
        <fnt><bib>MT</bib><lok>&Jer; 7:29</lok></fnt>;
      </ekz>
      <ekz>
        la maljunulo de<tld/>is <klr>[la foton]</klr> de la kovrilo
        kaj enkadrigis &gcirc;in
        <fnt>
        <aut>Wang Jianbing</aut>
        <vrk>El Popola &Ccirc;inio</vrk>
        <lok><url ref="http://www.china.org.cn/EL/EL/ElPopolaCxinio/bd98-8-5.html">Mia maljuna amiko Liu Bingjun</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>&ocirc;ter<klr tip="ind"> (en coupant)</klr></trd>,
    <trd>enlever<klr tip="ind"> (en coupant)</klr></trd>,
    <trd>d&eacute;couper<klr tip="ind"> (prendre en coupant avec des ciseaux)</klr></trd>,
    <trd>tailler<klr tip="ind"> (enlever en coupant)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>leny&iacute;r</trd>,
    <trd>lev&aacute;g <klr tip="ind">(oll&oacute;val)</klr></trd>
  </trdgrp>
</drv>
</art>
