<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tond.xml,v 1.30 2022/03/28 14:44:11 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tond</rad>/i
</kap>

<drv mrk="tond.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fortran&ccirc;i el io per <tld/>ilo:
      <ekz>
        <tld/>i la harojn, la barbon al iu;
      </ekz>
      <ekz>
        <tld/>i &scirc;afon, arbon;
      </ekz>
      <ekz>
        mallonge &gcirc;isnude <tld/>ita hundo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="raz.0i">razi</ref>,
      <ref cel="falcx.0i">fal&ccirc;i</ref>,
      <ref cel="tonsur.0o">tonsuro</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_r;&c_y;&c_g;&c_ch;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>ost&rcaron;&iacute;hat</trd>,
    <trd>st&rcaron;&iacute;hat</trd>
  </trdgrp>
  <trd lng="de">scheren</trd>
  <trd lng="en">shear</trd>
  <trd lng="es">cortar con tijera</trd>
  <trdgrp lng="fr">
    <trd>couper<klr tip="ind"> (aux ciseaux)</klr></trd>,
    <trd>d&eacute;couper<klr tip="ind"> (aux ciseaux)</klr></trd>,
    <trd>tondre<klr tip="ind"> (aux ciseaux)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ny&iacute;r</trd>,
    <trd>v&aacute;g<klr tip="ind"> (oll&oacute;val)</klr></trd>,
    <trd>nyes<klr tip="ind"> (oll&oacute;val)</klr></trd>
  </trdgrp>
  <trd lng="id">meng<ind>gunting</ind></trd>
  <trdgrp lng="nl">
    <trd>knippen</trd>,
    <trd>scheren</trd>,
    <trd>snoeien</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>strzyc<klr> (w&lstroke;osy)</klr></trd>,
    <trd>ci&aogonek;&cacute;<klr> (no&zdot;ycami)</klr></trd>,
    <trd>kraja&cacute;<klr> (no&zdot;ycami)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_i;&c_ch;&c_mol;</trd>
  <trd lng="sk">striha&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x442;&#x440;&#x438;&#x433;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x456;&#x434;&#x441;&#x442;&#x440;&#x438;&#x433;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x442;&#x44F;&#x442;&#x438;</trd>,
    <trd>&#x442;&#x43D;&#x443;&#x442;&#x438;</trd>,
    <trd>&#x43E;&#x431;&#x440;&#x456;&#x437;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="tond.0ilo">
  <kap><ofc>*</ofc><tld/>ilo
  <fnt><bib>F</bib>, <lok>Universala Vortaro, -il'</lok></fnt></kap>
  <snc>
    <dif>
      Ilo el du interkruci&gcirc;antaj almoveblaj
      klingoj, kies interna rando estas tran&ccirc;a:
      <ekz>
        per tondilo ni tondas
        <fnt><bib>F</bib> <lok>&FE; 34</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_zh;&c_n;&c_ib;&c_c;&c_y;</trd>
  <trd lng="cs">n&uring;&zcaron;ky</trd>
  <trd lng="de">Schere</trd>
  <trd lng="es">tijera</trd>
  <trd lng="fr">ciseaux</trd>
  <trd lng="hu">oll&oacute;</trd>
  <trd lng="id">gunting</trd>
  <trd lng="nl">schaar</trd>
  <trdgrp lng="pl">
    <trd>no&zdot;yce</trd>,
    <trd>no&zdot;yczki</trd>
  </trdgrp>
  <trd lng="ro">forfecu&tcedil;&abreve;</trd>
  <trd lng="ru">&c_n;&c_o;&c_zh;&c_n;&c_i;&c_c;&c_y;</trd>
  <trd lng="sk">no&zcaron;nice</trd>
  <trdgrp lng="uk">
    <trd>&#x43D;&#x43E;&#x436;&#x438;&#x446;&#x456;</trd>,
    <trd>&#x43C;&#x430;&#x448;&#x438;&#x43D;&#x43A;&#x430; &#x434;&#x43B;&#x44F; &#x441;&#x442;&#x440;&#x438;&#x436;&#x43A;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="tond.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="T"/>i &ccirc;iuflanke:
      <ekz>
        ne &ccirc;irka&ubreve;<tld/>u la flankojn de via kapo
        kaj ne fordifektu la flankojn de via barbo
        <fnt><bib>MT</bib><lok>&Lev; 19:27</lok></fnt>;
      </ekz>
      <ekz>
        hundeto, &ccirc;irka&ubreve;<tld/>ita tiel mallonge, ke oni
        povis vidi &gcirc;ian rozokoloran ha&ubreve;ton
        <fnt><bib>Fab1</bib><lok>Kupra porko</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">tondre</trd>
  <trdgrp lng="hu">
    <trd>k&ouml;rbev&aacute;g <klr tip="ind">(oll&oacute;val)</klr></trd>,
    <trd>k&ouml;rbeny&iacute;r</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ostrzyc</trd>,
    <trd>obcina&cacute;<klr> dooko&lstroke;a</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x431;&#x441;&#x442;&#x440;&#x438;&#x433;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43E;&#x431;&#x442;&#x438;&#x43D;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43E;&#x431;&#x440;&#x456;&#x437;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="tond.de0i">
  <kap>de<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Forigi <tld/>ante, preni <tld/>ante:
      <ekz>
        de<tld/>u viajn harojn kaj for&jcirc;etu ilin
        <fnt><bib>MT</bib><lok>&Jer; 7:29</lok></fnt>;
      </ekz>
      <ekz>
        la maljunulo de<tld/>is <klr>[la foton]</klr> de la kovrilo
        kaj enkadrigis &gcirc;in
        <fnt>
        <aut>Wang Jianbing</aut>
        <vrk>El Popola &Ccirc;inio</vrk>
        <lok><url ref="http://www.china.org.cn/EL/EL/ElPopolaCxinio/bd98-8-5.html">Mia
        maljuna amiko Liu Bingjun</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>odst&rcaron;ihnout</trd>,
    <trd>ust&rcaron;ihnout</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&ocirc;ter<klr tip="ind"> (en coupant)</klr></trd>,
    <trd>enlever<klr tip="ind"> (en coupant)</klr></trd>,
    <trd>d&eacute;couper<klr tip="ind"> (prendre en coupant avec des
    ciseaux)</klr></trd>,
    <trd>tailler<klr tip="ind"> (enlever en coupant)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>leny&iacute;r</trd>,
    <trd>lev&aacute;g <klr tip="ind">(oll&oacute;val)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wycina&cacute;<klr> (no&zdot;ycami)</klr></trd>,
    <trd>odcina&cacute;<klr> (no&zdot;ycami)</klr></trd>,
    <trd>odkraja&cacute;<klr> (no&zdot;ycami)</klr></trd>,
    <trd>odkrawa&cacute;<klr> (no&zdot;ycami)</klr></trd>
  </trdgrp>
  <trd lng="sk">odstrihn&uacute;&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x432;&#x456;&#x434;&#x442;&#x438;&#x43D;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x456;&#x434;&#x440;&#x456;&#x437;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="tond.gazon0ilo">
  <kap>
    gazon<tld/>ilo,
    <var><kap>razen<tld/>ilo</kap></var>
  </kap>
  <snc mrk="tond.gazon0ilo.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      Ma&scirc;ino por <tld/>i gazonon, herbejon:
      <ekz>
        unu el la radoj elfalis el la gazon<tld/>ilo kiam mi <tld/>is la gazonon
        <fnt>
          <aut>Pri Io Ajn</aut>
          <vrk><url ref="https://priioajn.wordpress.com/2019/05/22/riparinte-la-gazontondilon/">Riparinte la gazontondilon</url></vrk>
          <lok>2019</lok>
        </fnt>;
      </ekz>
      <ekz>
        la dinamo ekfunkcias kiel sputanta malnova razen<tld/>ilo, diras <nom>Sweet</nom>
        <fnt>
          <aut>C. Bowles, trad. D. Harlow</aut>
          <vrk><url ref="https://listserv.brown.edu/archives/cgi-bin/wa?A2=auxlang;614d24e7.0108E">&Ccirc;u la tera magneta kampo paneas?</url></vrk>
          <lok>2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_g;&c_r;&c_a;&c_d;&c_i;&c_n;&c_s;&c_k;&c_a; &c_k;&c_o;&c_s;&c_a;&c_ch;&c_k;&c_a;</trd>
  <trd lng="de">Rasenm&auml;her</trd>
  <trd lng="fr"><ind>tondeuse</ind> &agrave; gazon</trd>
  <trd lng="pl">kosiarka<klr> do trawy</klr></trd>
</drv>

<drv mrk="tond.gxarden0ilo">
  <kap>
    &gcirc;arden<tld/>ilo,
    <var><kap>&gcirc;ardena <tld/>ilo</kap></var>
  </kap>
  <snc mrk="tond.gxarden0ilo.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      Fortika <tld/>ilo, per kiu oni detran&ccirc;as bran&ccirc;etojn kaj similajn, ordinare kun
      mallongaj iom kurbaj klingoj:
      <ekz>
        li enmanigis al mi &gcirc;arden<tld/>ilon kaj igis min <tld/>i la&ubreve;ron
        <fnt>
          <aut>B. Bre&hcirc;to, trad. V. Lutermano</aut>
          <vrk><url ref="http://vlutermano.free.fr/kojno.html">Historioj pri sinjoro Kojno</url></vrk>
          <lok>[vidita en 2021]</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi tran&ccirc;is la&ubreve;longe duonige la botelon per &gcirc;ardena <tld/>ilo
        <fnt>
          <aut>E. Rolland</aut>
          <vrk><url ref="http://gxardeneto.chez-alice.fr/ex/akcepto.html">&Gcirc;ardenisto en la Lernejo</url></vrk>
          <lok>[vidita en 2021]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">s&eacute;cateur</trd>
</drv>
</art>
