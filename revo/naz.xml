<drv mrk="naz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="naz.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      <klr>(&ccirc;e vertebruloj)</klr> Kava flar<ref tip="super" cel="organ.0o.BIO">organo</ref> kun truoj,
      &ccirc;e la homo kaj multaj mamuloj elstaranta super la bu&scirc;o:
      <ekz>
        <klr>[mi]</klr> flaras per la <tld/>o
        <fnt>
          <bib>F</bib>
          <lok>&FE; 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        rekta
        <fnt>
          <bib>Metrop</bib>
        </fnt>,
        kurba
        <fnt>
          <bib>IdO</bib>
          <lok>Dua Parto</lok>
        </fnt>,
        plata
        <fnt>
          <bib>Fab3</bib>
          <lok>Kora sufero</lok>
        </fnt>
        <tld/>o;
      </ekz>
      <ekz>
        <tld/>o de porko <klr>(vd <ref tip="sub" cel="rostr.0o.ZOO">rostro</ref>)</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 11:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        la odoro agrable tiklis la <tld/>ojn de la gastoj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tablo, la orazeno kaj bastono el sako</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj estas faritaj por porti okulvitrojn, tial ni havas okulvitrojn
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi la <tld/>on portas tro super la nivelo
        <klr>(tro fieras)</klr>
        <fnt>
          <bib>LSB</bib>
          <lok>Averto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la pafado &ccirc;e Hornbergo, kaj ili devis foriri kun longa <tld/>o
        <klr>(elrevi&gcirc;is sen atingi ion)</klr>
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu fidas la krurojn de sia &ccirc;evalo, alia la <tld/>on de sia azeno
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        la nokto estis tiel malluma, ke ni nenion povis vidi e&ccirc; anta&ubreve; nia <tld/>o
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        preneton da tabako <klr>[&#8230;]</klr> la trankvilanima advokato metis <klr>[&#8230;]</klr>
        en la <tld/>on
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li devis jen kaj jen interrompi sin, purigi kaj vi&scirc;i la <tld/>on
        <fnt>
          <bib>VRA</bib>
          <lok>Eyvind Johnson</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i siblas tra la <tld/>o kaj ploretas
        <fnt>
          <bib>VRA</bib>
          <lok>Moa Martinson</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vila&gcirc;anino fermis la pordon anta&ubreve; lia <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la patrino donis al &scirc;i frapon sur la <tld/>on, tiel ke la <tld/>o fari&gcirc;is ru&gcirc;e
        kaj blue
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        grandaj balenoj &scirc;prucis akvon el la truoj de siaj <tld/>oj supren
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> diris <tld/>e, malbone prononcante la arameajn vortojn
        <klr>[&#8230;]</klr>
        <fnt>
          <bib>MkM</bib>
          <lok>2</lok>
        </fnt>;
      </ekz>
      <ekz>
        min akceptis fra&ubreve;lino kies okuloj pro &ccirc;iama mensogado strabis <tld/>en
        <fnt>
          <bib>MkM</bib>
          <lok>13</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        rigardu la faktojn, kiuj sur &ccirc;iu pa&scirc;o sin trovas anta&ubreve; via <tld/>o
        <fnt>
          <bib>EE</bib>
          <lok>V</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        videble mi havis bonan <tld/>on <klr>(mi &gcirc;uste divenis, konjektis, anta&ubreve;suspektis)</klr>,
        kaj lia najbareco tuj estis por mi suspekta
        <fnt>
          <bib>GDn</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>li vidas nur &gcirc;is la pinto de sia <tld/>o</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
        <trd lng="fr">ne pas voir plus loin que le bout de son nez</trd>
        <klr>(esti tute nekapabla anta&ubreve;vidi kaj anta&ubreve;zorgi, kp
            <ref tip="vid" cel="miop.0a">miopa</ref>)</klr>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ni orde kondukas ilin je la <tld/>o
        <klr>(arbitre persvadas, trompas)</klr>
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>en fremdan vazon ne &scirc;ovu la <tld/>on</ind>
        <trd lng="fr">m&ecirc;le-toi de ce qui te regarde</trd>
        <trd lng="ru">&c_n;&c_je; &c_s;&c_u;&c_j; &c_s;&c_v;&c_o;&c_j; &c_n;&c_o;&c_s; &c_v;
        &c_ch;&c_u;&c_zh;&c_o;&c_j; &c_v;&c_o;&c_p;&c_r;&c_o;&c_s;</trd>
        <klr>(ne enmiksu vin en aferojn, kiuj vin ne koncernas)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="muzel.0o">muzelo</ref>,
      <ref cel="naz.0ego">nazego</ref>
    </refgrp>
    <trd lng="bg">&c_n;&c_o;&c_s;</trd>
    <trd lng="ca">nas</trd>
    <trd lng="el">&my;&ypsilon_ton;&tau;&eta;</trd>
    <trd lng="fr">nez</trd>
    <trd lng="pt">nariz</trd>
    <trd lng="sv">n&auml;sa</trd>
  </snc>
<!--  <snc mrk="naz.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
