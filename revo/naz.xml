<drv mrk="naz.0a">
  <kap><tld/>a</kap>
  <snc mrk="naz.0a.gxenerale">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <uzo tip="fak">ANA</uzo>
        la <tld/>aj kavoj.
      </ekz>
    </dif>
  </snc>
  <snc mrk="naz.0a.FON">
    <uzo tip="fak">FON</uzo>
    <dif>
      <ref cel="naz.0sono">Nazsona</ref>:
      <ekz>
        <ref cel="naz.0akonsonanto"><tld/>a konsonanto</ref>,
      </ekz><ekz>
        <ref cel="naz.0avokalo"><tld/>a vokalo</ref>.
      </ekz>
    </dif>
    <ref tip="ant" cel="busx.0a.FON">bu&scirc;a<sncref/></ref>
  <trdgrp lng="id">
    <trd>nasal</trd>,
    <trd>sengau</trd>
  </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_o;&c_s;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_n;&c_a;&c_z;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nosn&iacute;</trd>,
    <trd>nosov&yacute;</trd>
  </trdgrp>
  <trd lng="en">nasal</trd>
  <trd lng="ja">&#x9F3B;&#x306E;  <pr>&#x306F;&#x306A;&#x306E;</pr></trd>
  <trd lng="ru">&c_n;&c_o;&c_s;&c_o;&c_v;&c_o;&c_j;</trd>
  <trd lng="sk">nosn&yacute;</trd>
  <trd lng="uk">&c_n;&c_o;&c_s;&c_o;&c_v;&c_i;&c_j;</trd>
  <trd lng="zh">&#x9F49; <pr>n&#xE0;ng</pr></trd>
</drv>

<drv mrk="naz.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="naz.0ego.ZOO">
    <dif>
      Dika <tld/>o de besto;
      <ref tip="sub" cel="muzel.0o.ZOO">muzelo</ref>:
      <ekz>
        <tld/>ego de porko
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ja;&c_l;&c_ib;&c_k;&c_ib; <ind>&c_n;&c_o;&c_s;</ind></trd>
    <trd>&c_ja;&c_n;&c_d;&c_o;&c_w;&c_k;&c_a; <klr tip="amb">(&c_n;&c_o;&c_s;)</klr></trd>,
    <trd>&c_b;&c_u;&c_l;&c_mol;&c_b;&c_ib;&c_n;&c_a; <klr tip="amb">(&c_n;&c_o;&c_s;)</klr></trd>
  </trdgrp>
  <trd lng="ca">musell</trd>
  <trdgrp lng="cs">
    <trd>&ccaron;enich</trd>,
    <trd>&ccaron;um&aacute;k</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>snout</trd>,
    <trd>muzzle</trd>
  </trdgrp>
  <trdgrp lng="fr">
        <trd>groin</trd>,
        <trd>mufle</trd>
        <trd>museau</trd>
      </trdgrp>
  <trdgrp lng="id">
    <trd>moncong</trd>,
    <trd>congor</trd>,
    <trd>cungur</trd>,
    <trd>jungur</trd>
  </trdgrp>
  <trd lng="pt">focinho</trd>
  <trdgrp lng="ru">
        <trd>&c_n;&c_o;&c_s;&c_i;&c_shch;&c_je;</trd>,
        <trd>&c_r;&c_y;&c_l;&c_o;</trd>
      </trdgrp>
  <trdgrp lng="sk">
    <trd>ra&tcaron;af&aacute;k</trd>,
    <trd>&ncaron;uf&aacute;k</trd>
  </trdgrp>
  <trd lng="sv">nos</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_i;&c_j; &c_n;&c_ib;&c_s;</trd>,
    <trd>&c_n;&c_o;&c_s;&c_i;&c_shch;&c_je;</trd>,
    <trd>&c_n;&c_o;&c_s;&c_ja;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#x543B;&#x90E8; <pr>w&#x11B;nb&#xF9;</pr></trd>
</drv>

<drv mrk="naz.0umo">
  <kap><ofc>*</ofc><tld/>umo <fnt><bib>F</bib>, <lok>Universala
  Vortaro, <url ref="&AdE;/fundamento/universala_vortaro.html#av-5119">naz'um'</url></lok></fnt></kap>
  <snc mrk="naz.0umo.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      <ref tip="super" cel="vitr.okul0oj">Okulvitroj</ref> fiksitaj
      sur la <tld/>o per pin&ccirc;a risorto;
      <ref tip="sin" cel="pincx.naz0ilo">nazpin&ccirc;ilo</ref>:
      <ekz>
        li elprenis <tld/>umon el ora ingo, vi&scirc;is &gcirc;in per ne&gcirc;blanka
        naztuko, metis &gcirc;in sur la nazon kaj kun vigle memkontenta
        rideto &ccirc;irka&ubreve;rigardis &ccirc;iujn sidantojn
        <fnt>
          <aut>I. G. &Scirc;irjaev</aut>,
          <vrk>Sen titolo</vrk>,
          <lok>[1898?]</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_n;&c_s;&c_n;&c_e;</trd>
  <trd lng="br">lunedo&ugrave;-fri</trd>
  <trd lng="ca">pin&ccedil;an&agrave;s</trd>
  <trdgrp lng="cs">
    <trd>cvikr</trd>,
    <trd>sk&rcaron;ipec</trd>
  </trdgrp>
    <trdgrp lng="de">
      <trd>Kneifer</trd>,
      <trd>Zwicker</trd>,
      <trd>Klemmer</trd>,
      <trd>Nasenklemmer</trd>,
      <trd>Zwickel</trd>
    </trdgrp>
  <trd lng="en">pince-nez</trd>
  <trd lng="fr">pince-nez</trd>
  <trdgrp lng="hu">
        <trd>cs&iacute;ptet&odblac;<klr tip="amb">(szem&uuml;vegfajta)</klr></trd>,
        <trd>cvikker</trd>
      </trdgrp>
  <trd lng="it">occhiali <ind>pince-nez</ind></trd>
  <trd lng="ja">&#x9F3B;&#x3081;&#x304C;&#x306D;  <pr>&#x306F;&#x306A;&#x3081;&#x304C;&#x306D;</pr></trd>
  <trd lng="nl">neusknijper</trd>
  <trd lng="pl">binokle</trd>
  <trd lng="pt">pincen&ecirc;</trd>
  <trd lng="ru">&c_p;&c_je;&c_n;&c_s;&c_n;&c_je;</trd>
  <trd lng="sk">cviker</trd>
  <trd lng="uk">&c_p;&c_je;&c_n;&c_s;&c_n;&c_je;</trd>
</drv>

<drv mrk="naz.alt0a">
  <kap>alt<tld/>a</kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc mrk="naz.alt0a.FIG">
    <dif>
      Tro memfiera,
      <ref tip="vid" cel="arogan.0a">aroganta</ref>,
      <ref tip="vid" cel="modest.mal0a.PSI">malmodesta</ref>:
      <ekz>
        la pse&ubreve;dofilozofo estas <klr>...</klr>
        despota kaj alt<tld/>a
        <fnt>
          <aut>V. O&scaron;lak</aut>,
          <vrk>Foje e&ccirc; pensi estas &gcirc;uo</vrk>,
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z; &c_z;&c_a;&c_d;&c_z;&c_jo;&c_r;&c_t;&c_y;&c_m; <ind>&c_n;&c_o;&c_s;&c_a;&c_m;</ind></trd>,
    <trd>&c_p;&c_y;&c_h;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_f;&c_a;&c_n;&c_a;&c_b;&c_e;&c_r;&c_y;&c_s;&c_t;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="en"><trd>nose-in-the-air</trd></trdgrp>
  <trd lng="fr">pr&eacute;tentiard</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_s;&c_o;&c_k;&c_o;&c_m;&c_je;&c_r;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_i;&c_r;&c_a;&c_ju;&c_shch;&c_i;&c_j; &c_n;&c_o;&c_s;</trd>
  </trdgrp>
</drv>

<drv mrk="naz.kusp0a">
  <kap>kusp<tld/>a</kap>
  <snc mrk="naz.kusp0a.ANA">
    <dif>
      Kies nazo (plejparte, mallonga) &scirc;ajnas turnita supren:
      <ekz>
        Sokrato estis kusp<tld/>a;
      </ekz><ekz>
        <nom>Ofelia Pepita</nom> estis <klr>(...)</klr> eble iomete tro
        kusp<tld/>a, sed ankora&ubreve; ne estas decidite, &ccirc;u tiu propreco
        estas makulo de beleco
        <fnt>
          <aut>J. Rejt&odblac;, trad. L. Bal&aacute;zs</aut>:
          <vrk><url ref="http://www.freeweb.hu/verkoj/fredlamalpura.htm">
              Fred la malpura intervenas</url></vrk>,
          <lok>2002</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_r;&c_p;&c_a;&c_n;&c_o;&c_s;&c_y;</trd>,
    <trd>&c_k;&c_ib;&c_r;&c_p;&c_a;&c_t;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">de nas arreveixinat</trd>
  <trd lng="en">snub-nosed</trd>
  <trd lng="fr">au <ind>nez en trompette</ind></trd>
  <trd lng="it">camuso</trd>
  <trd lng="ru">&c_k;&c_u;&c_r;&c_n;&c_o;&c_s;&c_y;&c_j;</trd>
</drv>

<drv mrk="naz.man0umo">
  <kap>man<tld/>umo</kap>
  <snc mrk="naz.man0umo.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      <ref tip="super" cel="vitr.okul0oj">Okulvitroj</ref>
      provizitaj per tenilo, kiujn oni mantenas kaj
      proksimigas al la viza&gcirc;o por trarigardi:
      <ekz>
        &gcirc;is la 19-a jarcento tondilformaj okulvitroj a&ubreve; man<tld/>umoj
        estis preferataj
        <klr>...</klr> man<tld/>umo estis anka&ubreve; brita inventa&jcirc;o, de iu
        <nom>George
        Adams</nom> en la 18-a jarcento
        <fnt>
          <aut>J. Warren</aut>
          <vrk><url ref="https://legacy.esperanto.org.uk/lbe/arkivo/970/09.html">
          Historio de okulvitroj</url></vrk>
          <lok>La Brita Esperantisto, 2011 printempo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_r;&c_n;&c_e;&c_t;</trd>
  <trd lng="en">lorgnette</trd>
  <trdgrp lng="fr">
    <trd>face &agrave; main</trd>,
    <trd>lorgnette</trd>
  </trdgrp>
</drv>

<drv mrk="naz.stump0a">
  <kap>stump<tld/>a</kap>
  <snc mrk="naz.stump0a.kuspa">
    <uzo tip="stl">EVI</uzo>
    <ref tip="dif" cel="naz.kusp0a.ANA">kusp<tld/>a</ref>
    <rim>
      La metaforo surbaze de stump- estas sufi&ccirc;e internacia, tamen
      &gcirc;i estas evitinda &ccirc;ar aliaj similaj kombina&jcirc;oj ofte indikas
      rezulton de kripligo (stumpa vosto, orelo ktp).
      <aut>Sergio Pokrovskij</aut>
    </rim>
    <trd lng="it">camuso</trd>
  </snc>
  <trd lng="cs">s tup&yacute;m nosem <klr>(adj.)</klr></trd>
  <trd lng="sk">s tup&yacute;m nosom</trd>
  <trd lng="uk">&c_k;&c_i;&c_r;&c_p;&c_a;&c_t;&c_i;&c_j;</trd>
</drv>

<drv mrk="naz.0sono">
  <kap><tld/>sono</kap>
  <snc mrk="naz.0sono.FON">
    <uzo tip="fak">FON</uzo>
    <dif>
      <ref tip="super" cel="son.parol0o">Parolsono</ref> dum kies
      artikulacio la velo estas mallevita kaj interkomunikigas
      la bu&scirc;on kun la <tld/>a kavo:
      <ekz>
        la franca lingvo vere ne bezonas iri trans la
        pura<klr>n</klr>
        rimo<klr>n</klr>, &gcirc;ian rimadon ja enviinde helpas la
        <tld/>sonoj,
        kiuj englutas post si &ccirc;iujn konsonantojn
        <fnt>
          <aut>K. Kalocsay</aut>:
          <vrk>Kiel verki kaj traduki poemojn?</vrk>.
          <lok>Budapest: HEA, 1979.</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="naz.0akonsonanto"><tld/>a konsonanto</ref>,
      <ref cel="naz.0avokalo"><tld/>a vokalo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_s;&c_a;&c_v;&c_y; &c_g;&c_u;&c_k;</trd>
  <trd lng="ca">nasal <klr>(fon.)</klr></trd>
  <trd lng="cs">nosovka</trd>
  <trdgrp lng="de">
        <trd>Nasenlaut</trd>,
        <trd>Nasal</trd>
      </trdgrp>
  <trd lng="en">nasal</trd>
  <trd lng="es">nasal</trd>
  <trd lng="fr">nasale<klr> (subst., consonne ou voyelle)</klr></trd>
  <trdgrp lng="hu">
        <trd>orrhang</trd>,
        <trd>naz&aacute;lis</trd>
      </trdgrp>
  <trd lng="id">nasal</trd>
  <trd lng="ja">&#x9F3B;&#x97F3;  <pr>&#x3073;&#x304A;&#x3093;</pr></trd>
  <trd lng="nl">neusklank</trd>
  <trd lng="ru">&c_n;&c_o;&c_s;&c_o;&c_v;&c_o;&c_j; &c_z;&c_v;&c_u;&c_k;</trd>
  <trd lng="sk">nosovka</trd>
  <trd lng="zh">&#x9F3B;&#x97F3; <pr>b&#xED;y&#x12B;n</pr></trd>
</drv>

<drv mrk="naz.0akonsonanto">
  <kap><tld/>a konsonanto</kap>
  <snc mrk="naz.0akonsonanto.FON">
    <uzo tip="fak">FON</uzo>
    <dif>
      <ref tip="super" cel="konson1.0o">Konsonanto</ref>
      kiu estas <ref tip="super" cel="naz.0sono"><tld/>sono</ref>:
      <ekz>
        du fonemoj esperantaj estas <tld/>aj konsonantoj:
        /m/ kaj /n/.
      </ekz><ekz>
        N estas denta <tld/>a konsonanto
        <fnt><bib>PMEG</bib>,
          <lok><url ref="&PMEG;skribo_elparolo/elparolo/bazaj_reguloj.html#i-nol">Konsonanta
          variado</url></lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_s;&c_a;&c_v;&c_y; &c_z;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="en">nasal consonant</trd>
  <trd lng="fr">consonne nasale</trd>
  <trdgrp lng="id">
    <trd><ind>konsonan</ind> nasal</trd>,
    <trd><ind>konsonan</ind> sengau</trd>
  </trdgrp>
  <trd lng="pl">sp&oacute;&lstroke;g&lstroke;oska nosowa</trd>
  <trd lng="ru">&c_n;&c_o;&c_s;&c_o;&c_v;&c_o;&c_j; &c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="naz.0avokalo">
  <kap><tld/>a vokalo</kap>
  <snc mrk="naz.0avokalo.FON">
    <uzo tip="fak">FON</uzo>
    <dif>
      <ref tip="super" cel="vokal.0o">Vokalo</ref>
      kiu estas <ref tip="super" cel="naz.0sono"><tld/>sono</ref>:
      <ekz>
        en la malnovslava ekzistis <tld/>aj vokaloj...
        <fnt>
          <aut>Joh&aacute;n Valano</aut>:
          <vrk>&Ccirc;u ni kunvenis vane?</vrk>.
          <lok><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0042/0017.php">&Ccirc;ap.
          17&#xAA;</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_s;&c_a;&c_v;&c_y; &c_g;&c_a;&c_l;&c_o;&c_s;&c_n;&c_y;</trd>
  <trd lng="de">Nasalvokal</trd>
  <trd lng="en">nasal vowel</trd>
  <trd lng="fr">voyelle nasale</trd>
  <trdgrp lng="id">
    <trd><ind>vokal</ind> nasal</trd>,
    <trd><ind>vokal</ind> sengau</trd>
  </trdgrp>
  <trd lng="pl">samog&lstroke;oska nosowa</trd>
  <trd lng="ru">&c_n;&c_o;&c_s;&c_o;&c_v;&c_o;&c_j; &c_g;&c_l;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>
  <trd lng="zh">&#x9F3B;&#x5316;&#x5143;&#x97F3; <pr>b&#xED;hu&#xE0;yu&#xE1;ny&#x12B;n</pr></trd>
</drv>

