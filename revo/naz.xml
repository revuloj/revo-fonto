<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>naz</rad>/o
</kap>
<drv mrk="naz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="naz.0o.homo">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Flarorgano elstaranta el la kapo inter frunto kaj bu&scirc;o:
      <ekz>
        rekta,
        kurba <tld/>o;
      </ekz>
      <ekz>
        <ind><tld/>truoj</ind>;
        <trdgrp lng="br">
          <trd>divfronell</trd>,    
          <trd>fronello&ugrave;</trd>
        </trdgrp> 
        <trd lng="fr">narines</trd>           
      </ekz>
      <ekz>
        <tld/>loboj;
      </ekz>
      <ekz>
        <ind><tld/>kataro</ind>;   
        <trd lng="br">friad<klr tip="ind"> (kle&ntilde;ved)</klr></trd> 
        <trdgrp lng="fr">
          <trd>coryza</trd>,    
          <trd>rhinite</trd>, 
          <trd>rhume de cerveau</trd>
        </trdgrp> 
      </ekz>
      <ekz>
        purigi la <tld/>on;
      </ekz>
      <ekz>
        <tld/>e, tra la <tld/>o paroli;
      </ekz>
      <ekz>
        <tld/>asona vo&ccirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>aj sonoj, vokaloj (nazalo);
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ne &scirc;ovu la <tld/>on en fremdan vazon
        <klr>(ne enmiksu vin en aferoj, kiuj ne koncernas vin.
        <refgrp tip="sin">
          <ref cel="en.0igxi">eni&gcirc;i</ref>,
          <ref cel="zorg.0i">zorgi</ref>
        </refgrp>)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        vidi nur &gcirc;is la pinto de sia <tld/>o<fnt>Z</fnt>
        <klr>(esti tute nekapabla anta&ubreve;vidi kaj anta&ubreve;zorgi.
        <ref tip="sin" cel="miop.0a">miopa</ref>)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        rigardu la faktojn, kiuj sin trovas anta&ubreve; via
        <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi havis bonan <tld/>on<fnt>Z</fnt>
        <klr>(mi &gcirc;uste divenis, konjektis, anta&ubreve;suspektis)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        fari longan <tld/>on<fnt>Z</fnt>, foriri kun longa
        <tld/>o<fnt>Z</fnt>
        <klr>(montri konsternitan mienon pro elrevi&gcirc;o,
        ne atinginte la esperitan rezultaton)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        teni la <tld/>on supren<fnt>Z</fnt>
        <klr>(esti arogante fiera)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        konduki iun per<fnt>B</fnt>, &ccirc;e, je la nazo<fnt>Z</fnt>
        <klr>(la&ubreve; sia arbitro.
        <ref tip="sin" cel="laux">la&ubreve; I</ref>)</klr>.
      </ekz>
    </dif>
    <trd lng="el">&my;&ypsilon_ton;&tau;&eta;</trd>
    <trd lng="fr">nez</trd>
    <trd lng="pt">nariz</trd>
  </snc>
  <snc mrk="naz.0o.besto">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Sama organo &ccirc;e la bestoj:
      <ekz>
        <tld/>o de hundo, rato.
      </ekz>
    </dif>
    <trd lng="fr">museau</trd>
    <trd lng="pt">focinho</trd>
  </snc>
  <snc mrk="naz.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Elstaranta parto de objekto pli malpli simila al <tld/>o:
      <ekz>
        &scirc;ip<tld/>o
        <klr>(anta&ubreve;a&jcirc;o, pruo)</klr>;
      </ekz>
      <ekz>
        <tld/>o de plugilo;
      </ekz>
      <ekz>
        <tld/>eto de kru&ccirc;o.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>nez <klr>(d'un objet)</klr></trd>,
      <trd>pointe</trd>
    </trdgrp>
    <trd lng="pt">nariz</trd>
  </snc>
  <trd lng="be">&c_n;&c_o;&c_s;</trd>             
  <trd lng="br">fri</trd>
  <trd lng="cs">nos</trd>
  <trd lng="de">Nase</trd>
  <trd lng="en">nose</trd>
  <trd lng="es">nariz</trd>
  <trd lng="hu">orr</trd>  
  <trd lng="nl">neus</trd>
  <trd lng="pl">nos</trd>
  <trd lng="ru">&c_n;&c_o;&c_s;</trd>
  <trd lng="tr">burun</trd>
</drv>

<drv mrk="naz.0umo">
  <kap><tld/>umo</kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="vitr.okul0oj">Okulvitroj</ref> fiksitaj sur la
      <tld/>o per pinta risorto.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="pincx.naz0ilo">nazpin&ccirc;ilo</ref>
  </refgrp>
  <trd lng="be">&c_p;&c_e;&c_n;&c_s;&c_n;&c_e;</trd>          
  <trd lng="br">lunedo&ugrave;-fri</trd>
  <trd lng="de">Kneifer</trd>
  <trd lng="en">pince-nez</trd>
  <trd lng="fr">pince-nez</trd>
  <trdgrp lng="hu">
    <trd>cs&iacute;ptet&odblac;<klr tip="amb">(szem&uuml;vegfajta)</klr></trd>,
    <trd>cvikker</trd>
  </trdgrp>
  <trd lng="nl">neusknijper</trd>
  <trd lng="pl">binokle</trd>   
  <trd lng="pt">pincen&ecirc;</trd>
  <trd lng="ru">&c_p;&c_je;&c_n;&c_s;&c_n;&c_je;</trd>
</drv>
</art>
