      Dika <tld/>o de besto;
      <ref tip="sub" cel="rostr.0o">rostro</ref>,
      <ref tip="sub" cel="muzel.0o.ZOO">muzelo</ref>:
      <ekz>
        <tld/>ego de porko
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ja;&c_l;&c_ib;&c_k;&c_ib; <ind>&c_n;&c_o;&c_s;</ind></trd>
    <trd>&c_ja;&c_n;&c_d;&c_o;&c_w;&c_k;&c_a; <klr tip="amb">(&c_n;&c_o;&c_s;)</klr></trd>,
    <trd>&c_b;&c_u;&c_l;&c_mol;&c_b;&c_ib;&c_n;&c_a; <klr tip="amb">(&c_n;&c_o;&c_s;)</klr></trd>
  </trdgrp>
  <trd lng="ca">musell</trd>
  <trdgrp lng="cs">
    <trd>&ccaron;enich</trd>,
    <trd>&ccaron;um&aacute;k</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>snout</trd>,
    <trd>muzzle</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>groin</trd>,
    <trd>mufle</trd>
    <trd>museau</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>moncong</trd>,
    <trd>congor</trd>,
    <trd>cungur</trd>,
    <trd>jungur</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>nochal</trd>,
    <trd>chrapy <klr>(zoo)</klr></trd>
  </trdgrp>
  <trd lng="pt">focinho</trd>
  <trdgrp lng="ru">
        <trd>&c_n;&c_o;&c_s;&c_i;&c_shch;&c_je;</trd>,
        <trd>&c_r;&c_y;&c_l;&c_o;</trd>
      </trdgrp>
  <trdgrp lng="sk">
    <trd>ra&tcaron;af&aacute;k</trd>,
    <trd>&ncaron;uf&aacute;k</trd>
  </trdgrp>
  <trd lng="sv">nos</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_i;&c_j; &c_n;&c_ib;&c_s;</trd>,
    <trd>&c_n;&c_o;&c_s;&c_i;&c_shch;&c_je;</trd>,
    <trd>&c_n;&c_o;&c_s;&c_ja;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#21563;&#37096; <pr>w&ecaron;nb&ugrave;</pr></trd>
</drv>

<drv mrk="naz.0umo">
  <kap><ofc>*</ofc><tld/>umo</kap>
  <snc mrk="naz.0umo.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      <ref tip="super" cel="vitr.okul0oj">Okulvitroj</ref> fiksitaj
      sur la <tld/>o per pin&ccirc;a risorto;
      <ref tip="sin" cel="pincx.naz0ilo">nazpin&ccirc;ilo</ref>:
      <ekz>
        li elprenis <tld/>umon el ora ingo, vi&scirc;is &gcirc;in per ne&gcirc;blanka
        naztuko, metis &gcirc;in sur la nazon kaj kun vigle memkontenta
        rideto &ccirc;irka&ubreve;rigardis &ccirc;iujn sidantojn
        <fnt>
          <aut>I. G. &Scirc;irjaev</aut>,
          <vrk>Sen titolo</vrk>,
          <lok>[1898?]</lok></fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/2/2b/Knijpbril_of_pince-nez_met_ovale_glazen_op_sterkte%2C_montuur_van_koper_met_hoge_brug_en_neussteunen_uit_&eacute;&eacute;n_stuk%2C_objectnr_71141.JPG/640px-Knijpbril_of_pince-nez_met_ovale_glazen_op_sterkte%2C_montuur_van_koper_met_hoge_brug_en_neussteunen_uit_&eacute;&eacute;n_stuk%2C_objectnr_71141.JPG"
      prm="CC BY-SA 3.0"
      ><tld/>umo
