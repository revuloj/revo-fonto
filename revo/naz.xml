    <trd><ind>vokal</ind> sengau</trd>
  </trdgrp>
  <trd lng="pl">samog&lstroke;oska <ind>nosowa</ind></trd>
  <trd lng="ru">&c_n;&c_o;&c_s;&c_o;&c_v;&c_o;&c_j; &c_g;&c_l;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>
  <trd lng="zh">&#40763;&#21270;&#20803;&#38899; <pr>b&iacute;hu&agrave;yu&aacute;ny&imacron;n</pr></trd>
</drv>
