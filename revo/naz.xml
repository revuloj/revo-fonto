  <snc mrk="naz.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Anta&ubreve;a, elstara parto de objekto pli malpli simila al <tld/>o;
      <ref tip="vid" cel="bek.0o.FIG">beko<sncref/></ref>:
      <ekz>
        &scirc;ip<tld/>o
        <klr>(anta&ubreve;a&jcirc;o, pruo)</klr>;
      </ekz><ekz>
        <tld/>o de plugilo;
      </ekz><ekz>
        <tld/>eto de kru&ccirc;o.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>punta</trd>,
      <trd>sortint</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>nez <klr>(d'un objet)</klr></trd>,
