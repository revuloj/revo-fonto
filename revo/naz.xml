<drv mrk="naz.0a">
  <kap><tld/>a</kap>
  <snc mrk="naz.0a.gxenerale">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <uzo tip="fak">ANA</uzo>
        la <tld/>aj kavoj.
      </ekz>
    </dif>
  </snc>
  <snc mrk="naz.0a.FON">
    <uzo tip="fak">FON</uzo>
    <dif>
      <ref cel="naz.0sono">Nazsona</ref>:
      <ekz>
        <ref cel="naz.0akonsonanto"><tld/>a konsonanto</ref>,
      </ekz><ekz>
        <ref cel="naz.0avokalo"><tld/>a vokalo</ref>;
      </ekz>
      <ekz>
        en praktiko oni ordinare anta&ubreve; &leftquot;g&rightquot; a&ubreve; &leftquot;k&rightquot; elparolas la sonon &leftquot;n&rightquot; <tld/>e
        <fnt>
          <bib>LR</bib>
          <lok>Dua Parto &ndash; Gramatiko</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="busx.0a.FON">bu&scirc;a<sncref/></ref>
    <trdgrp lng="id">
      <trd>nasal</trd>,
      <trd>sengau</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_o;&c_s;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_n;&c_a;&c_z;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nosn&iacute;</trd>,
    <trd>nosov&yacute;</trd>
  </trdgrp>
  <trd lng="en">nasal</trd>
  <trd lng="ja">&#40763;&#12398;  <pr>&#12399;&#12394;&#12398;</pr></trd>
  <trdgrp lng="pl">
    <trd>nosowy</trd>,
    <trd>nazalny</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_o;&c_s;&c_o;&c_v;&c_o;&c_j;</trd>
  <trd lng="sk">nosn&yacute;</trd>
