<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dong.xml,v 1.14 2023/08/31 10:44:01 revo Exp $">
<kap>
  <rad>dong</rad>/o
  <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="dong.0o">
  <kap><tld/>o</kap>
  <snc mrk="dong.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="lst" cel="unu.mon0o" lst="voko:monunuoj">Monunuo</ref> 
      de Vjetnamo:
      <ekz>
        &scirc;ia monata salajro estas 300000 <tld/>oj
        <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>&leftquot;la
        socialismo de nudeloj&rightquot;, p. 50a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_o;&c_n;&c_g;</trd>
  <trd lng="ca">dong</trd>
  <trd lng="cs">dong</trd>
  <trdgrp lng="de">
    <trd>Dong</trd>,
    <trd>&#x110;&#x1ED3;ng</trd>    
  </trdgrp>
  <trd lng="fr">dong<klr tip="amb"> (monnaie vietnamienne)</klr></trd>
  <trd lng="hu">dong<klr tip="amb"> (p&eacute;nznem)</klr></trd>
  <trd lng="id">dong</trd>
  <trd lng="ja">&#x30C9;&#x30F3;</trd>
  <trd lng="nl">dong<klr tip="ind"> (munteenheid)</klr></trd>
  <trd lng="pl">dong<klr> (waluta)</klr></trd>
  <trd lng="pt">dong</trd>
  <trd lng="sk">dong</trd>
  <trdgrp lng="uk">
    <trd>&#x434;&#x43E;&#x43D;&#x433;</trd>,
    <trd>&#x434;&#x43E;&#x43D;&#x456;</trd>
  </trdgrp>
  <trd lng="vi">&#x110;&#x1ED3;ng</trd>
</drv>
</art>

