<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cxif.xml,v 1.25 2023/08/31 10:10:28 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>&ccirc;if</rad>/i
</kap>
<drv mrk="cxif.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Senorde krispe <ref tip="super" cel="prem.kun0i.KOMUNE">kunpremi</ref> ion, 
      farante multajn faldojn:
      <ekz>
        <tld/>i veston, teksa&jcirc;on, paperon;
      </ekz>
      <ekz>
        lia mano <tld/>is nerve la muslinon
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia manoj agititaj de nerva movado <tld/>is konvulsie la malfeli&ccirc;an kartonon
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>as la monon en la po&scirc;on
        <fnt>
          <bib>LGA</bib>
          <lok>III.</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun furioza dentogrinco kaj murmuro li <tld/>is, dis&scirc;iris kaj for&jcirc;etis la gazeton
        <fnt>
          <bib>LGA</bib>
          <lok>I.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Vitalij</nom> embarase <tld/>is en la manoj la oficialan leteron de la &gcirc;enerala
        kancelario
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Zoja</nom>, emociigita, nerveme <tld/>is la verdan, inkomakulitan tablodrapon
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la kliento kolere <tld/>is sian &ccirc;apon
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bul.0igi">buligi</ref>,
      <ref cel="fald.0i">faldi</ref>,
      <ref cel="kned.0i">knedi</ref>,
      <ref cel="krisp.0igi">krispigi</ref>,
      <ref cel="tauxz.0i">ta&ubreve;zi</ref>,
      <ref cel="tord.0i">tordi</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_m;&c_ja;&c_c;&c_mol;</trd>,
      <trd>&c_k;&c_a;&c_m;&c_ja;&c_ch;&c_y;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>arrugar</trd>,
      <trd>rebregar</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>zerknittern</trd>,
      <trd>zerkn&uuml;llen</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>crumple</trd>,
      <trd>wrinkle</trd>
    </trdgrp>
    <trd lng="es">arrugar</trd>
    <trdgrp lng="fr">
      <trd>chiffonner</trd>,
      <trd>froisser</trd>,
      <trd>friper</trd></trdgrp>
    <trdgrp lng="hu">
      <trd>gy&udblac;r</trd>,
      <trd>&ouml;sszegy&udblac;r</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>stropicciare</trd>,
      <trd>sgualcire</trd>
    </trdgrp>
    <trdgrp lng="nl">
       <trd>verkreukelen</trd>,
       <trd>verfrommelen</trd>
    </trdgrp>
    <trdgrp lng="pl">
       <trd>mi&eogonek;tosi&cacute;</trd>,
       <trd>gnie&sacute;&cacute;</trd>,
       <trd>mi&aogonek;&cacute;</trd>
    </trdgrp>
    <trd lng="pt">amarrotar</trd>
    <trdgrp lng="ru">
      <trd>&c_m;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_k;&c_o;&c_m;&c_k;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="he">&lamed;&qof;&mem;&tet;</trd>
  <trd lng="ja">&#x3057;&#x308F;&#x304F;&#x3061;&#x3083;&#x306B;&#x3059;&#x308B;</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&#x2019;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_g;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_ib;&c_zh;&c_m;&c_a;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="cxif.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
  <dif>
    Io <tld/>ita:
      <ekz>
        li plene <tld/>is la tola&jcirc;on a&ubreve; la paperon, poste li pentris supron de la
        <tld/>a&jcirc;o, post iom da tempo li glatigis &gcirc;in kaj aperis senformaj makuloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=480745">Simon Hantai</url></lok>
        </fnt>.
      </ekz>
  </dif>
  <refgrp tip="vid">
    <ref cel="cxifon.0o">&ccirc;ifono</ref>
  </refgrp>
  </snc>
<trd lng="be">&c_k;&c_a;&c_m;&c_ja;&c_k;</trd>
<trd lng="ca">rebrec</trd>
<trdgrp lng="de">
  <trd>Zerknittertes</trd>,
  <trd>Zerkn&uuml;lltes</trd>
</trdgrp>
<trd lng="es">arruga</trd>
<trd lng="he">&qof;&#x5B6;&mem;&#x5B6;&tet;</trd>
  <trdgrp lng="hu">
  <trd>galacsin</trd>,
  <trd>pap&iacute;rcsom&oacute;</trd>
</trdgrp>
<trd lng="it">straccio<klr> (cosa straccia)</klr></trd>
<trd lng="ja">&#x3057;&#x308F;&#x306B;&#x306A;&#x3063;&#x305F;&#x7269;  [<ind>&#x3057;&#x308F;&#x306B;&#x306A;&#x3063;&#x305F;&#x3076;&#x3064;</ind>]</trd>
  <trdgrp lng="nl">
  <trd>iets dat <ind>verkreukeld</ind> is</trd>,
  <trd>iets dat <ind>verfrommeld</ind> is</trd>
</trdgrp>
<trdgrp lng="pl">
  <trd>zwitek</trd>
</trdgrp>
<trd lng="ru">&c_k;&c_o;&c_m;&c_o;&c_k;</trd>
  <trd lng="uk">&c_zh;&c_m;&c_u;&c_t;</trd>
</drv>

<drv mrk="cxif.ne0ebla">
  <kap>ne<tld/>ebla</kap>
  <snc>
  <dif>
    Tiel preparita, ke oni ne povas &gcirc;in <tld/>i:
    <ekz>
      ne<tld/>ebla silko.
    </ekz>
  </dif>
  <ref tip="sin" cel="glad.ne0enda">negladenda</ref>
  <trd lng="ca">no arrugad&iacute;s</trd>
  <trdgrp lng="en">
    <trd>wrinkle-free</trd>,
    <trd>no-iron</trd>
  </trdgrp>
  <trd lng="es">inarrugable</trd>
  <trd lng="fr">infroissable</trd>
  <trd lng="it">ingualcibile</trd>
  <trd lng="pl">niemn&aogonek;cy</trd>  
  </snc>
  <trd lng="de">knitterfrei</trd>
  <trd lng="ja">&#x3057;&#x308F;&#x306B;&#x306A;&#x3089;&#x306A;&#x3044;</trd>
  <trd lng="uk">&c_ja;&c_k;&c_i;&c_j; &c_n;&c_je; &c_m;&c_n;&c_je;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

</art>
