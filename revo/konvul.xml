<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>konvulsi</rad>/o
</kap>
<drv mrk="konvul.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Subita nevola stre&ccirc;o de muskoloj, kiu akompanas iujn malsanajn
      statojn de nerva sistemo:
      <ekz>
        <tld/>o de infano;
      </ekz>
      <ekz>
        <tld/>a tusado.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Konvulsion</trd>,
      <trd>Zuckungskrampf</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Fortega skuado de la viza&gcirc;aj trajtoj a&ubreve; de partoj de la korpo
      ka&ubreve;zita de forta emocio:
      <ekz>
        la <tld/>o de malespero stre&ccirc;is lin;
      </ekz>
      <ekz>
        la brakoj etendi&gcirc;is supren per <tld/>a movo;
      </ekz>
      <ekz>
        mano <tld/>e kunpremita.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="spasm.0o">spasmo</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Spasmische Bewegung</trd>,
      <trd>Spasmus</trd>,
      <trd>Zuckungskrampf</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_p;&c_a;&c_z;&c_m;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_n;&c_v;&c_u;&c_l;&c_mol;&c_s;&c_ib;&c_ja;</trd>,
    <trd>&c_s;&c_u;&c_t;&c_a;&c_r;&c_g;&c_a;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_z;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>g&ouml;rcs</trd>,
    <trd>vonagl&aacute;s</trd>,
    <trd>konvulzi&oacute;</trd>
  </trdgrp>  
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_d;&c_o;&c_r;&c_o;&c_g;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_v;&c_u;&c_l;&c_mol;&c_s;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="konvul.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Havi <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>g&ouml;rcse van</trd>,
    <trd>g&ouml;rbcsben van</trd>
    <trd>vonaglik</trd>,
    <trd>r&aacute;ngat&oacute;dzik</trd>
  </trdgrp>  
</drv>
</art>

