<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: konvul.xml,v 1.21 2023/12/03 09:58:57 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>konvulsi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="konvul.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="konvul.0i.MED">
    <dif>
      Subite agiti&gcirc;i, <ref tip="super" cel="mov.0i">movi</ref>
      korpopartojn pro pretervola muskola kuntiri&gcirc;o:
      <ekz>
        la demono lin &jcirc;etis sur la teron kaj lin <tld/>igis
        <fnt>
          <bib>NT</bib>
          <lok>Luko 9:42</lok>
        </fnt>;
      </ekz>
      <ekz>
        la birdo etendis siajn belajn flugilojn, ankora&ubreve; unu fojon <tld/>e ektremis
        <klr>[&#x2026;]</klr> kaj &gcirc;ia koro krevis de senforteco
        <fnt>
          <bib>Fab1</bib>
          <lok>Lekanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e &scirc;i palpadis &ccirc;irka&ubreve; si per la mano, faris pa&scirc;on en la direkto al la tero
        kaj falis senkonscie
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        li sentis, ke &scirc;i <tld/>e ektremas, kaj li tenis &scirc;in des pli forte
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">zucken</trd>
  <trd lng="en">convulse</trd>
  <trd lng="fr">convulser</trd>
  <trdgrp lng="hu">
    <trd>g&ouml;rcse van</trd>,
    <trd>g&ouml;rbcsben van</trd>
    <trd>vonaglik</trd>,
    <trd>r&aacute;ngat&oacute;dzik</trd>
  </trdgrp>  
  <trdgrp lng="ja">
    <trd>&#x3051;&#x3044;&#x308C;&#x3093;&#x3059;&#x308B;</trd>,
    <trd>&#x3072;&#x304D;&#x3064;&#x3051;&#x3092;&#x8D77;&#x3053;&#x3059;  <pr>&#x3072;&#x304D;&#x3064;&#x3051;&#x3092;&#x304A;&#x3053;&#x3059;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>drga&cacute;</trd>,
    <trd>konwulsje mie&cacute;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>tremura</trd>,
    <trd>au convulsii</trd>
  </trdgrp>
</drv>

<drv mrk="konvul.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="konvul.0o.MED">
  <uzo tip="fak">MED</uzo>
    <dif>
      Subita nevola <ref tip="super" cel="strecx.0o">stre&ccirc;o</ref>
      de muskoloj, kiu akompanas iujn malsanajn statojn de nerva sistemo:
      <ekz>
        <tld/>o de infano;
      </ekz>
      <ekz>
        <tld/>a tusado;
      </ekz>
      <ekz>
        <klr>[li]</klr> falis, skuata de la anta&ubreve;mortaj <tld/>oj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Konvulsion</trd>,
      <trd>Zuckung</trd>
    </trdgrp>
  </snc>
  <snc mrk="konvul.0o.spasmo">
    <dif>
      <ref tip="dif" cel="spasm.0o">Spasmo</ref>:
      <ekz>
        <tld/>o de malespero stre&ccirc;is lin;
      </ekz>
      <ekz>
        la brakoj etendi&gcirc;is supren per <tld/>a movo;
      </ekz>
      <ekz>
        mano <tld/>e kunpremita;
      </ekz>
      <ekz>
        min trafis plor<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>J&uacute;lia Sigmond</aut>
          <vrk><url ref="&Monato;/2009/009693.php">La mistera nokto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        aperas la tipa, tre turmenta, simptomo: akvotimego (tial rabio ankora&ubreve; nomi&gcirc;as
        hidrofobio), kiam &ccirc;e la malsanulo dum iu provo eltrinki e&ccirc; unu guton de akvo,
        aperas <tld/>aj kunstre&ccirc;igoj de faringomuskoloj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2008/009550.php">Batalo kontra&ubreve; mortiga malsano</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="kramf.0o">kramfo</ref>
    <ref tip="malprt" cel="epilep.0o">epilepsio</ref>
    <ref tip="vid" cel="klonik.0aspasmo">klonika spasmo</ref>
    <trdgrp lng="de">
      <trd>Spasmische Bewegung</trd>,
      <trd>Spasmus</trd>,
      <trd>Zuckungskrampf</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_p;&c_a;&c_z;&c_m;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_n;&c_v;&c_u;&c_l;&c_mol;&c_s;&c_ib;&c_ja;</trd>,
    <trd>&c_s;&c_u;&c_t;&c_a;&c_r;&c_g;&c_a;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_z;&c_m;</trd>
  </trdgrp>
  <trd lng="bo">&#xF60;&#xF46;&#xF74;&#xF66;&#xF0B;&#xF54;&#xF0B;</trd>
  <trd lng="en">convulsion</trd>
  <trd lng="fr">convulsion</trd>
  <trdgrp lng="hu">
    <trd>g&ouml;rcs</trd>,
    <trd>vonagl&aacute;s</trd>,
    <trd>konvulzi&oacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3051;&#x3044;&#x308C;&#x3093;</trd>,
    <trd>&#x3072;&#x304D;&#x3064;&#x3051;</trd>,
    <trd/>,
    <trd>&#x3053;&#x308F;&#x3070;&#x308A;</trd>,
    <trd>&#x9707;&#x3048;  <pr>&#x3075;&#x308B;&#x3048;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>konwulsje [l.mn.]</trd>,
    <trd>drgawka</trd>
  </trdgrp>   
  <trd lng="pt">convuls&atilde;o <klr tip="amb">(med.)</klr></trd> 
  <trdgrp lng="ro">
    <trd>convulsii</trd>,
    <trd>spasm</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_d;&c_o;&c_r;&c_o;&c_g;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_v;&c_u;&c_l;&c_mol;&c_s;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x75C9;&#x631B; <pr>j&#xEC;nglu&#xE1;n</pr></trd>,
    <trd>&#x7628; <pr>di&#x101;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="konvul.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="konvul.0ulo.MED">
    <dif>
      Suferanto pro <tld/>oj:
      <ekz>
        la suno tremetis kiel <tld/>ulo
        <fnt>
          <aut>Mo Yan, trad. Vejdo</aut>
          <vrk>Travidebla Ru&gcirc;a Rafano</vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">convulsionnaire<klr> (subst.)</klr></trd>
  <trd lng="pl">wstrz&aogonek;sany konwulsjami</trd>     
  <trd lng="ro">scuturat de convulsii</trd>
</drv>
</art>

