<drv mrk="opini.dis0i">
  <kap>dis<tld/>i,
  <var><kap>malsam<tld/>i</kap></var></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="opini.dis0i.KOMUNE">
    <dif>
      Havi malsamajn, diversajn <tld/>ojn:
      <ekz>
        mi traktos pri punktoj, pri kiuj homoj dis<tld/>as
        <fnt>
          <bib>BonaLingvo</bib>
          <lok>1. Enkonduke</lok>
        </fnt>;
      </ekz>
      <ekz>
        plenkreskuloj diskutas kaj malsam<tld/>as
        <fnt>
          <aut>J. Schram</aut>,
          <vrk>Eble la fama Esperanto-amikeco...</vrk>,
          <bib>LOdE</bib>,
          <lok>2003:8-9 (106-107)</lok>
        </fnt>;
      </ekz>
      <ekz>
        tamen &ccirc;iu partio dis<tld/>as pri kio estas la patrujo
        <fnt>
          <aut>J. Salgero</aut>,
          <vrk><url ref="http://www.nodo50.org/esperanto/artik34.htm">Militista parado</url></vrk>.
          <lok>Sennaciulo, 2004-12</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="difere.0i">diferenci</ref>,
      <ref cel="disput.0i">disputi</ref>,
      <ref cel="akord.mal0i">malakordi</ref>,
      <ref cel="konsen.mal0i">malkonsenti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_y;&c_h;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_c;&c_a; &c_w; <ind>&c_m;&c_je;&c_r;&c_k;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ja;&c_h;</ind></trd>,
    <trd>&c_m;&c_je;&c_c;&c_mol; <ind>&c_r;&c_o;&c_z;&c_n;&c_a;&c_g;&c_a;&c_l;&c_o;&c_s;&c_mol;&c_s;&c_ib;</ind></trd>
  </trdgrp>
  <trd lng="cs">m&iacute;t odli&scaron;n&yacute; n&aacute;zor</trd>
  <trdgrp lng="de">
    <trd>unterschiedlicher <ind>Meinung</ind> sein</trd>,
    <trd><ind>uneinig</ind> sein</trd>
  </trdgrp>
  <trd lng="fr">&ecirc;tre d'un autre <ind>avis</ind></trd>
  <trd lng="id"><baz>beda</baz>berbeda pendapat</trd>
  <trdgrp lng="nl">
    <trd>van <ind>mening</ind> verschillen</trd>,
    <trd>een tegengestelde <ind>mening</ind> hebben</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd><ind>uwa&zdot;a&cacute;</ind> inaczej</trd>,
    <trd>mie&cacute; inne <ind>zdanie</ind></trd>,
    <trd>mie&cacute; odmienne <ind>zdanie</ind></trd>,
    <trd>opiniowa&cacute; inaczej</trd>,
    <trd>s&aogonek;dzi&cacute; inaczej</trd>
  </trdgrp>
