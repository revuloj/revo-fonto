    <trd><baz>dapat</baz>pendapat umum</trd>,
    <trd><ind>opini</ind> publik</trd>,
    <trd><ind>opini</ind> umum</trd>
  </trdgrp>
  <trd lng="ja">&#x901A;&#x8AAC;  <pr>&#12388;&#12358;&#x305B;&#12388;</pr></trd>
