  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_p;&c_a;&c_r;&c_t;&c_y;</trd>,
    <trd>&c_s;&c_h;&c_ib;&c_l;&c_mol;&c_n;&c_y; &c_d;&c_a; <ind>&c_s;&c_p;&c_r;&c_e;&c_ch;&c_a;&c_k;</ind></trd>
  </trdgrp>
  <trd lng="de">tendenzi&ouml;s</trd>
  <trdgrp lng="fr">
