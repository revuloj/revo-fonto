<art mrk="$Id: gluten.xml,v 1.18 2022/03/19 19:18:23 revo Exp $">
<kap>
  <rad>gluten</rad>/o
</kap>
<drv mrk="gluten.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Glueca proteina&jcirc;o, tre nutra, kiu trovi&gcirc;as en la faruno de
      cerealoj:
      <ekz>
        preska&ubreve; &ccirc;iuj kutimaj bieroj enhavas pro la uzataj cerealoj <tld/>on kaj tiel ne
        utilas por homoj, kiuj suferas <tld/>alergion, tamen per uzo de
        ne-<tld/>enhavaj cerealoj, kiel ekzemple maizo, rizo, milio, fagopiro a&ubreve;
        sorgo, eblas fari sen<tld/>an bieron
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=602">Biero</url></lok>
        </fnt>.
      </ekz>  
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_l;&c_je;&c_j;&c_k;&c_a;&c_v;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_g;&c_l;&c_ju;&c_t;&c_e;&c_n;</trd>
  </trdgrp>
  <trd lng="bg">&c_g;&c_l;&c_u;&c_t;&c_je;&c_n;</trd>
  <trd lng="ca">gluten</trd>
  <trdgrp lng="cs">
    <trd>gluten</trd>,
    <trd>lepek</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Gluten</trd>,
     <trd>Klebereiwei&szlig;</trd></trdgrp>
  <trd lng="en">gluten</trd>
  <trd lng="es">gluten</trd>
  <trd lng="fa">&f_g;&a_l;&a_w;&a_t;&a_n;</trd>
  <trd lng="fr">gluten</trd>
  <trdgrp lng="hu">
     <trd>sik&eacute;r</trd>,
     <trd>glut&eacute;n</trd>
  </trdgrp>
  <trd lng="nl">gluten</trd>
  <trdgrp lng="pt">
   <trd>gl&uacute;ten</trd>, 
   <trd>glute</trd> 
  </trdgrp>
  <trd lng="ru">&c_k;&c_l;&c_je;&c_j;&c_k;&c_o;&c_v;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>glut&eacute;n</trd>,
    <trd>lepok</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&#x43B;&#x435;&#x439;&#x43A;&#x43E;&#x432;&#x438;&#x43D;&#x430;</trd>
</drv>
</art>
