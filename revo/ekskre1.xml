<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ekskre1.xml,v 1.18 2015/07/04 08:37:33 revo Exp $">
<kap>
<ofc>1</ofc>
<rad>ekskrement</rad>/o
</kap>
<drv mrk="ekskre1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Iu ajn el la substancoj forkondukitaj el la organismo
      <klr>(
      <refgrp tip="sub">
        <ref cel="fek.0ajxo">feka&jcirc;o</ref>,
        <ref cel="urin.0o">urino</ref>,
        <ref cel="sxvit.0o">&scirc;vito</ref>,
        <ref cel="gal.0o">galo</ref>,
        ktp
      </refgrp>
      )</klr>; plejofte feka&jcirc;o.
    </dif>
    <refgrp tip="vid">
      <ref cel="pur">malpura&jcirc;o</ref>,
      <ref cel="rekrem.0o">rekremento</ref>,
      <ref cel="fecx.0o">fe&ccirc;o</ref>.
    </refgrp>
  </snc>
  <trd lng="ca">excrement</trd>
  <trdgrp lng="cs">
    <trd>exkrement</trd>,
    <trd>stolice</trd>,
    <trd>trus</trd>,
    <trd>výkal</trd>,
    <trd>výmět<klr>(ek)</klr></trd>,
    <trd>výměšky</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Exkrement</trd>,
     <trd>Ausscheidungsprodukt</trd>
  </trdgrp>
  <trd lng="en">excrement</trd>
  <trd lng="es">excremento</trd>
  <trd lng="fr">excr&eacute;ment</trd>
  <trd lng="hu">&uuml;r&uuml;l&eacute;k</trd>
  <trd lng="it">escremento</trd>
  <trd lng="pl">ekskrement</trd>
  <trd lng="pt">excremento</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_l;</trd>,
    <trd>&c_e;&c_k;&c_s;&c_k;&c_r;&c_je;&c_m;&c_je;&c_n;&c_t;&c_y;</trd>,
    <trd>&c_i;&c_s;&c_p;&c_r;&c_a;&c_zh;&c_n;&c_je;&c_n;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>trus</trd>,
    <trd>vylúčená látka</trd>,
    <trd>výkal</trd>
  </trdgrp>
</drv>
<drv mrk="ekskre1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o.
    </dif>
    <trdgrp lng="ca">
      <trd>excrementici</trd>,
      <trd>excremental</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>excremental</trd>,
      <trd>excrementitious</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>exkrementierend</trd>,
      <trd>ausscheidend</trd>
   </trdgrp>
   <trdgrp lng="fr">
      <trd>excr&eacute;mentiel</trd>,
      <trd>f&eacute;cal</trd>
    </trdgrp>
    <trd lng="hu">exkrement&aacute;lis</trd>
    <trd lng="it">escrementizio</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malestiminda, mal&scirc;atinda:
      <ekz>
        <tld/>a animo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kot.0a">kota</ref>.
    </refgrp>
    <trdgrp lng="ca">
      <trd>immund</trd>,
      <trd>infecte</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>f&auml;kalisch</trd>
   </trdgrp>
    <trd lng="fr">de <ind>chiotte</ind></trd>
    <trdgrp lng="hu">
      <trd>mocskos</trd>,
      <trd>s&aacute;ros</trd>,
      <trd>szaros</trd></trdgrp>
    <trd lng="it">putrido</trd>
  </snc>
</drv>
</art>
