<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>ekskrement</rad>/o
</kap>
<drv mrk="ekskre1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Iu ajn el la substancoj forkondukitaj el la organismo
      <klr>(
      <refgrp tip="sub">
        <ref cel="fek.0ajxo">feka&jcirc;o</ref>,
        <ref cel="urin.0o">urino</ref>,
        <ref cel="sxvit.0o">&scirc;vito</ref>,
        <ref cel="gal.0o">galo</ref>,
        ktp
      </refgrp>
      )</klr>; plejofte feka&jcirc;o.
    </dif>
    <refgrp tip="vid">
      <ref cel="pur">malpura&jcirc;o</ref>,
      <ref cel="rekrem.0o">rekremento</ref>,
      <ref cel="fecx.0o">fe&ccirc;o</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Exkrement</trd>,
     <trd>Ausscheidungsprodukt</trd>
  </trdgrp>
  <trd lng="es">excremento</trd>
  <trd lng="fr">excr&eacute;ment</trd>
  <trd lng="en">excrement</trd>
  <trd lng="hu">&uuml;r&uuml;l&eacute;k</trd>
  <trd lng="pl">ekskrement</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_l;</trd>,
    <trd>&c_e;&c_k;&c_s;&c_k;&c_r;&c_je;&c_m;&c_je;&c_n;&c_t;&c_y;</trd>,
    <trd>&c_i;&c_s;&c_p;&c_r;&c_a;&c_zh;&c_n;&c_je;&c_n;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>
<drv mrk="ekskre1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o.
    </dif>
    <trdgrp lng="en">
      <trd>excremental</trd>,
      <trd>excrementitious</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>exkrementierend</trd>,
      <trd>ausscheidend</trd>
   </trdgrp>
   <trdgrp lng="fr">
      <trd>excr&eacute;mentiel</trd>,
      <trd>f&eacute;cal</trd>
    </trdgrp>
    <trd lng="hu">exkrement&aacute;lis</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malestiminda, mal&scirc;atinda:
      <ekz>
        <tld/>a animo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kot.0a">kota</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>f&auml;kalisch</trd>
   </trdgrp>
    <trd lng="fr">de <ind>chiotte</ind></trd>
    <trdgrp lng="hu">
      <trd>mocskos</trd>,
      <trd>s&aacute;ros</trd>,
      <trd>szaros</trd></trdgrp>
  </snc>
</drv>
</art>
