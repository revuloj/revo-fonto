<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ekskre1.xml,v 1.25 2023/11/07 19:22:36 revo Exp $">
<kap>
<ofc>1</ofc>
<rad>ekskrement</rad>/o
</kap>
<drv mrk="ekskre1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Iu ajn el la substancoj forkondukitaj el la organismo
      <klr>(<refgrp tip="sub">
        <ref cel="fek.0ajxo">feka&jcirc;o</ref>,
        <ref cel="urin.0o">urino</ref>,
        <ref cel="sxvit.0o">&scirc;vito</ref>,
        <ref cel="gal.0o">galo</ref>
        </refgrp>, ktp)</klr>;
      plejofte feka&jcirc;o:
      <ekz>
        uzu ekskrementojn de brutoj <klr>[&#x2026;]</klr> kaj sur ili pretigu vian panon
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 4:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni purigis <klr>[la arenon]</klr> de la kadavroj, de sango kaj <tld/>oj
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        vi estas nenia&jcirc;o, sterko, <tld/>o
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
          <lok>7. La nesolvebla problemo.</lok>
        </fnt>!
      </ekz>
      <ekz>
        &ccirc;e birdoj la gripvirusoj reprodukti&gcirc;as en la intestaro kaj eli&gcirc;as precipe kun la
        <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2003/008099.html">Birda gripo, &ccirc;u dan&gcirc;era por la homo?</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ojn oni efike reuzas por sterki
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo DURWAEL</aut>
          <vrk><url ref="&Monato;/2004/008236.html">La rikolto de la nova tempo</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pur">malpura&jcirc;o</ref>,
      <ref cel="neces.0ajxo.ekskremento">necesa&jcirc;o<sncref/></ref>,
      <ref cel="rekrem.0o">rekremento</ref>,
      <ref cel="fecx.0o">fe&ccirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="ca">excrement</trd>
  <trdgrp lng="cs">
    <trd>exkrement</trd>,
    <trd>stolice</trd>,
    <trd>trus</trd>,
    <trd>v&yacute;kal</trd>,
    <trd>v&yacute;m&ecaron;t<klr>(ek)</klr></trd>,
    <trd>v&yacute;m&ecaron;&scaron;ky</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Exkrement</trd>,
     <trd>Ausscheidungsprodukt</trd>
  </trdgrp>
  <trd lng="en">excrement</trd>
  <trd lng="es">excremento</trd>
  <trd lng="fr">excr&eacute;ment</trd>
  <trd lng="hu">&uuml;r&uuml;l&eacute;k</trd>
  <trd lng="it">escremento</trd>
  <trdgrp lng="ja">
    <trd>&#x7CDE;&#x4FBF;  <pr>&#x3075;&#x3093;&#x3079;&#x3093;</pr></trd>,
    <trd>&#x5927;&#x4FBF;  <pr>&#x3060;&#x3044;&#x3079;&#x3093;</pr></trd>,
    <trd>&#x304F;&#x305D;</trd>,
    <trd>&#x3075;&#x3093;</trd>
  </trdgrp>
  <trd lng="pl">ekskrement</trd>
  <trd lng="pt">excremento</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_l;</trd>,
    <trd>&c_e;&c_k;&c_s;&c_k;&c_r;&c_je;&c_m;&c_je;&c_n;&c_t;&c_y;</trd>,
    <trd>&c_i;&c_s;&c_p;&c_r;&c_a;&c_zh;&c_n;&c_je;&c_n;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>trus</trd>,
    <trd>vyl&uacute;&ccaron;en&aacute; l&aacute;tka</trd>,
    <trd>v&yacute;kal</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_l;</trd>,
    <trd>&c_je;&c_k;&c_s;&c_k;&c_r;&c_je;&c_m;&c_je;&c_n;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_p;&c_o;&c_r;&c_o;&c_zh;&c_n;&c_je;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
  <trd lng="zh">&#x7CAA;&#x4FBF; <pr>f&#xE8;nbi&#xE0;n</pr></trd>
</drv>
<drv mrk="ekskre1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o.
    </dif>
    <trdgrp lng="ca">
      <trd>excrementici</trd>,
      <trd>excremental</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>excremental</trd>,
      <trd>excrementitious</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>exkrementierend</trd>,
      <trd>ausscheidend</trd>
   </trdgrp>
   <trdgrp lng="fr">
      <trd>excr&eacute;mentiel</trd>,
      <trd>f&eacute;cal</trd>
    </trdgrp>
    <trd lng="hu">exkrement&aacute;lis</trd>
    <trd lng="it">escrementizio</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="estim.mal0inda">Malestiminda</ref>, mal&scirc;atinda:
      <ekz>
        <tld/>a animo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kot.0a">kota</ref>
    </refgrp>
    <trdgrp lng="ca">
      <trd>immund</trd>,
      <trd>infecte</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>f&auml;kalisch</trd>,
      <trd>beschissen</trd>
    </trdgrp>
    <trd lng="fr">de <ind>chiotte</ind></trd>
    <trdgrp lng="hu">
      <trd>mocskos</trd>,
      <trd>s&aacute;ros</trd>,
      <trd>szaros</trd></trdgrp>
    <trd lng="it">putrido</trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x7CDE;&#x4FBF;&#x306E; <pr>&#x3075;&#x3093;&#x3079;&#x3093;&#x306E;</pr></trd>,
    <trd>&#x6C5A;&#x3089;&#x3057;&#x3044;  <pr>&#x304D;&#x305F;&#x306A;&#x3089;&#x3057;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="uk">&c_f;&c_je;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j;</trd>
</drv>
</art>
