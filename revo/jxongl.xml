<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&jcirc;ongl</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="jxongl.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="jxongl.0i.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Lerte &jcirc;eti en aeron diversajn objektojn kaj rekapti por ilin tuj
      re&jcirc;eti.
    </dif>
  </snc>
  <snc mrk="jxongl.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Lerte ludi:
      <ekz>
        <tld/>i per la rimoj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akroba">akrobati</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_n;&c_g;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">jonglieren</trd>
  <trd lng="en">juggle</trd>
  <trd lng="es">hacer <ind>malabarismos</ind></trd>
  <trd lng="fr">jongler</trd>
  <trd lng="nl">jongleren</trd>
  <trd lng="hu">zsongl&odblac;rk&ouml;dik</trd>
  <trd lng="pt">fazer malabarismos</trd>
  <trd lng="ru">&c_zh;&c_o;&c_n;&c_g;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">jonglera</trd>
</drv>

<drv mrk="jxongl.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="jxongl.0isto.SPO">
    <dif>
      Profesia <tld/>anto, lerta amuzisto.
    </dif>
    <refgrp tip="vid">
      <ref cel="iluzi.0isto">iluziisto</ref>,
      <ref cel="presti.0o">prestidigitatoro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_n;&c_g;&c_l;&c_jo;&c_r;</trd>
  <trd lng="de">Jongleur</trd>
  <trd lng="en">juggler</trd>
  <trd lng="es">malabarista</trd>
  <trd lng="el">&tau;&alfa;&chi;&ypsilon;&delta;&alfa;&kappa;&tau;&ypsilon;&lambda;&omikron;&ypsilon;&rho;&gamma;&omikron_ton;&sigma_fina;</trd>
  <trd lng="fr">jongleur</trd>
  <trd lng="hu">zsongl&odblac;r</trd>
  <trd lng="it">giocoliere</trd>
  <trd lng="nl">jongleur</trd>
  <trd lng="pt">malabarista</trd>
  <trd lng="ru">&c_zh;&c_o;&c_n;&c_g;&c_l;&c_jo;&c_r;</trd>
  <trd lng="sv">jongl&ouml;r</trd>
</drv>

<drv mrk="jxongl.for0i">
  <kap>for<tld/>i</kap>
  <snc mrk="jxongl.for0i.KOMUNE">
    <dif>
      Lerte forigi, for<ref tip="vid" cel="kasx.0i.KOMUNE">ka&scirc;i</ref>:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;i neniel aludis mian viziton.
        Probable oni &gcirc;in for<tld/>is de &scirc;ia scio
        <fnt><aut>R. Radiguet, trad. M. Duc Goninaz</aut>, <vrk>Kun 
        Diablo en la korpo</vrk>, <lok>Mondial, 2006</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">escamoter</trd>
</drv>
</art>
