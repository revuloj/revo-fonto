<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>2</ofc>
   <rad>obol</rad>/o
</kap>
<drv mrk="obol.0o">
   <kap><tld/>o</kap>
   <fnt><bib>PIV1</bib></fnt>
   <fnt><bib>KrED</bib></fnt>
   <uzo tip="fak">EKON</uzo>
   <uzo tip="fak">HIS</uzo>
   <snc>
     <dif>
       Antikva greka <ref tip="super" cel="mon.0ero">monereto</ref>.
     </dif>
   </snc>
  <trd lng="be">&c_a;&c_b;&c_o;&c_l;</trd>
  <trdgrp lng="cs">
    <trd>obol</trd>,
    <trd>obolos</trd>,
    <trd>obolus</trd>,
    <trd>starořecký peníz</trd>
  </trdgrp>
  <trd lng="de">Obolus</trd>
  <trd lng="el">&omikron;&beta;&omikron;&lambda;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">obolus</trd>
  <trd lng="fr">obole</trd>
  <trd lng="hu">obulus</trd>
  <trd lng="nl">obool</trd>
  <trd lng="pl">obol</trd>
  <trd lng="pt">&oacute;bolo</trd>
  <trd lng="ru">&c_o;&c_b;&c_o;&c_l;</trd>
  <trdgrp lng="sk">
    <trd>obolus</trd>,
    <trd>starogrécky peniaz</trd>
  </trdgrp>
</drv>
</art>
