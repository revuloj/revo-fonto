<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: obol.xml,v 1.26 2025/03/08 08:26:24 revo Exp $">
<kap>
   <ofc>2</ofc>
   <rad>obol</rad>/o
   <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="obol.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">EKON</uzo>
   <uzo tip="fak">HIS</uzo>
   <snc mrk="obol.0o.monero">
     <dif>
       Antikva greka <ref tip="super" cel="mon.0ero">monereto</ref>, sesono de
       <ref tip="malprt" cel="drahxm.0o">dra&hcirc;mo</ref>:
      <ekz>
        kie estas la tempo, kiam por <tld/>o oni povis ricevi tiom da fabo kun lardo,
        kiom oni povis teni en amba&ubreve; manoj
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <snc mrk="obol.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="malprt" cel="almoz.0o">Almozo</ref>, modesta sumeto ks:
      <ekz>
        fra&ubreve;lino, vi havas sepdek du gradojn da nobeleco, kaj ne e&ccirc; unu <tld/>on
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        orgojlaj &jcirc;urnalistoj, strebantaj perlabora&ccirc;i sian <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Sergej Derevjanny&hcirc;</aut>
          <vrk><url ref="&Monato;/2004/008512.html">Horora</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        al la &leftquot;<tld lit="O"/>o de Sankta Petro&#8221;, konsistanta el la oferoj destinitaj helpi
        la papon en liaj misioj apostola kaj monasista, fluis 79 milionoj da usonaj dolaroj
        <fnt>
          <bib>Monato</bib>
          <aut>Armando Zecchin</aut>
          <vrk><url ref="&Monato;/2008/009589.php">La kontoj
          de Vatikano kaj de la Sankta Se&gcirc;o</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kopek.0o">kopeko</ref>
    </refgrp>
    <trd lng="de">Scherflein</trd>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_o;&c_l;</trd>
  <trdgrp lng="cs">
    <trd>obol</trd>,
    <trd>obolos</trd>,
    <trd>obolus</trd>,
    <trd>staro&rcaron;eck&yacute; pen&iacute;z</trd>
  </trdgrp>
  <trd lng="de">Obolus</trd>
  <trd lng="el">&omikron;&beta;&omikron;&lambda;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">obolus</trd>
  <trd lng="fr">obole</trd>
  <trd lng="hu">obulus</trd>
  <trd lng="ja">&#12458;&#12508;&#12525;&#12473;</trd>
  <trd lng="nl">obool</trd>
  <trd lng="pl">obol</trd>
  <trd lng="pt">&oacute;bolo</trd>
  <trd lng="ru">&c_o;&c_b;&c_o;&c_l;</trd>
  <trdgrp lng="sk">
    <trd>obolus</trd>,
    <trd>starogr&eacute;cky peniaz</trd>
  </trdgrp>
  <trd lng="uk">&c_o;&c_b;&c_o;&c_l; (&c_m;&c_o;&c_n;&c_je;&c_t;&c_a;)</trd>
  <trdgrp lng="zh">
    <trd>&#22885;&#27874;&#21202;&#26031; <pr>&agrave;ob&omacron;l&emacron;is&imacron;</pr></trd>,
    <trd>&#22887;&#27874;&#21202;&#26031; <pr>&agrave;ob&omacron;l&emacron;is&imacron;</pr></trd>
  </trdgrp>
</drv>
</art>
