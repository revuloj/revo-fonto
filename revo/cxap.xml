<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>&ccirc;ap</rad>/o
</kap>
<drv mrk="cxap.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="vest.kap0o">Kapvesto</ref> sen elstara rando kaj sen viziero:
      <ekz>
        bekhava <tld/>o;
      </ekz>
      <ekz>pli kara kapo ol <tld/>o<fnt><bib>PrV</bib></fnt>.</ekz> 
    </dif>
    <refgrp tip="vid">
      <ref cel="kuf.0o">kufo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de"><trd>Kappe</trd>, <trd>M&uuml;tze</trd></trdgrp>
  <trd lng="en">cap</trd>
  <trd lng="fr">bonnet</trd>
  <trd lng="nl">muts</trd>
  <trd lng="ru">&c_sh;&c_a;&c_p;&c_k;&c_a;</trd>
  <trd lng="sv">m&ouml;ssa</trd>
</drv>
<drv mrk="cxap.verto0o">
  <kap>verto<tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Eta ronda duonsfera <ref tip="super" cel="cxap.0o"><tld/>eto </ref> kovranta nur la verton, uzata interalie
      de katolikaj pastroj.
    </dif>
  </snc>
  <trd lng="en">skull<ind> cap</ind></trd>
  <trd lng="fr">calotte<klr tip="ind"> (coiffure)</klr></trd>
  <trd lng="nl">kalotje</trd>
</drv>
</art>
