<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>&ccirc;ap</rad>/o
</kap>
<drv mrk="cxap.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Kapvesto sen elstara rando k sen viziero:
      <ekz>
        bekhava <tld/>o;
      </ekz>
      <ekz>
        pli kara kapo ol <tld/>o
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>.
      </ekz> 
    </dif>
    <refgrp tip="sin">
      <ref cel="kuf.0o">kufo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de"><trd>Kappe</trd>, <trd>M&uuml;tze</trd></trdgrp>
  <trd lng="en">cap</trd>
  <trd lng="ru">&c_sh;&c_a;&c_p;&c_k;&c_a;</trd>
</drv>
<drv mrk="cxap.verto0o">
  <kap>verto<tld/>o</kap>
  <snc>
    <dif>
      Ronda duonsfera <tld/>eto kovranta nur la verton, uzata de katolikaj
      pastroj.
    </dif>
  </snc>
</drv>
</art>
