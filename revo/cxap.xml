      <trd>pokrywa</trd>,
      <trd>skuwka</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_y;&c_sh;&c_k;&c_a;</trd>,
      <trd>&c_k;&c_o;&c_l;&c_p;&c_a;&c_ch;&c_o;&c_k;</trd>,
      <trd>&c_p;&c_o;&c_k;&c_r;&c_y;&c_sh;&c_k;&c_a;<klr>
      (&c_a;&c_v;&c_t;.)
    </klr></trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#20445;&#25252;&#30422; <pr>b&#462;oh&ugrave;g&agrave;i</pr></trd>,
    <trd>&#20445;&#38505;&#32617; <pr>b&#462;oxi&#462;nzh&agrave;o</pr></trd>,
    <trd>&#24125; <pr>m&agrave;o</pr></trd>
  </trdgrp>
</snc>
