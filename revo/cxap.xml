<drv mrk="cxap.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="cxap.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="vest.kap0o">Kapvesto</ref>
      sen &ccirc;irka&ubreve;a rando:
      <ekz>
        la vento forblovis de mia kapo la <tld/>on
        <fnt>
          <bib>F</bib>
          <lok>&FE; 39</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#8230;]</klr> ne portis la altan &ccirc;apelon, sed <tld/>on, kaj sub &gcirc;i ankora&ubreve;
        unu broditan <klr>[&#8230;]</klr> noktan <tld/>eton
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o el ursa felo sidis sur lia kapo &gcirc;is sub la oreloj
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o havis enfleksitan &scirc;irmilon, kaj <klr>[&#8230;]</klr> estis tiel aran&gcirc;ita
        tiel praktike, ke oni povis &scirc;ovi &gcirc;in en la po&scirc;on
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref tip="sub" cel="kalot.0o.cxapeto">kalota</ref> <tld/>o
        <fnt>
          <aut>Gilbert Ledon</aut>
          <vrk><url ref="&Monato;/2000/007175.php">Muroj dumiljaraj solidas</url></vrk>
          <bib>Monato</bib>
          <lok>2000/11, p. 5</lok>
          </fnt>;
      </ekz>
      <ekz>
        ofte <tld/>o de kampulo kovras kapon de sa&gcirc;ulo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>pli kara kapo ol <tld/>o</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="hu">k&ouml;zelebb az ing, mint a suba</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxapel.0o.kapvesto">&ccirc;apelo</ref>,
      <ref cel="kuf.0o">kufo</ref>
    </refgrp>
    <trd lng="be">&c_sh;&c_a;&c_p;&c_k;&c_a;</trd>
    <trdgrp lng="de">
      <trd>M&uuml;tze</trd>,
      <trd>Kappe</trd>
    </trdgrp>
    <trd lng="en">cap</trd>
    <trd lng="fr">bonnet</trd>
    <trd lng="hu">sapka</trd>
    <trd lng="it">berretto</trd>
    <trd lng="nl">muts</trd>
    <trd lng="pl">czapka</trd>
    <trdgrp lng="pt">
      <trd>barrete</trd>,
      <trd>bon&eacute;</trd>
    </trdgrp>
    <trd lng="ro">bonet&abreve;</trd>
    <trd lng="ru">&c_sh;&c_a;&c_p;&c_k;&c_a;</trd>
    <trd lng="sk">&ccaron;iapka</trd>
    <trd lng="sv">m&ouml;ssa</trd>
    <trdgrp lng="zh">
    <trd>&#32701;&#20896; <pr>y&#468;gu&amacron;n</pr></trd>,
    <trd>&#35199;&#29916;&#30382; <pr>x&imacron;gu&amacron;p&iacute;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="cxap.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      Similforma &scirc;irmanta objekto:
      <ekz>
        <tld/>o de skribilo;
      </ekz>
      <ekz>
        <tld/>oj el <klr>[&#8230;]</klr> fumo super la kamentuboj
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>;
      </ekz>
      <ekz>
        sur iliaj pintoj staras &scirc;ton<tld/>oj <klr>[&#8230;]</klr> kiel pluvombreloj
        <fnt>
          <bib>Monato</bib>
          <aut>Dimitrije Jani&ccaron;i&cacute;</aut>
          <vrk><url ref="&Monato;/2008/009444.php">Diabla urbeto</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o kiu protektas la pinton de la radiko
        <fnt>
          <vrk><url ref="https://www.jardineriaon.com/eo/partoj-de-la-plantoj.html">Kiuj estas la
          partoj de plantoj?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_sh;&c_a;&c_p;&c_k;&c_a;</trd>,
      <trd>&c_n;&c_a;&c_k;&c_r;&c_y;&c_w;&c_k;&c_a;</trd>,
      <trd>&c_k;&c_a;&c_w;&c_p;&c_a;&c_k;</trd>,
      <trd>&c_k;&c_a;&c_p;&c_t;&c_u;&c_r;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>tap</trd>,
      <trd>caputx&oacute;</trd>
    </trdgrp>
    <trd lng="en">cap</trd>
    <trdgrp lng="fr">
      <trd>capuchon<klr tip="ind"> (fermeture)</klr></trd>,
      <trd>bouchon<klr tip="ind"> (capuchon)</klr></trd>,
      <trd>enjoliveur</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Kappe</trd>,
      <trd>Schutzkappe</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kupak</trd>,
      <trd>sapka</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>kap</trd>,
      <trd>tutup<klr tip="amb"> (pulpen dsb.)</klr></trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>cappuccio</trd>,
      <trd>tappo</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>os&lstroke;ona</trd>,
