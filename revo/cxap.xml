<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cxap.xml,v 1.82 2021/06/18 20:41:28 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&ccirc;ap</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="cxap.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="cxap.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="vest.kap0o">Kapvesto</ref>
      sen &ccirc;irka&ubreve;a rando:
      <ekz>
        la vento forblovis de mia kapo la <tld/>on
        <fnt>
          <bib>F</bib>
          <lok>&FE; 39</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#x2026;]</klr> ne portis la altan &ccirc;apelon, sed <tld/>on, kaj sub &gcirc;i ankora&ubreve;
        unu broditan <klr>[&#x2026;]</klr> noktan <tld/>eton
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o el ursa felo sidis sur lia kapo &gcirc;is sub la oreloj
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o havis enfleksitan &scirc;irmilon, kaj <klr>[&#x2026;]</klr> estis tiel aran&gcirc;ita
        tiel praktike, ke oni povis &scirc;ovi &gcirc;in en la po&scirc;on
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref tip="sub" cel="kalot.0o.cxapeto">kalota</ref> <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Gilbert Ledon</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007175.html">Muroj dumiljara solidas</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ofte <tld/>o de kampulo kovras kapon de sa&gcirc;ulo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>pli kara kapo ol <tld/>o</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="hu">k&ouml;zelebb az ing, mint a suba</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxapel.0o.kapvesto">&ccirc;apelo</ref>,
      <ref cel="kuf.0o">kufo</ref>
    </refgrp>
    <trd lng="be">&c_sh;&c_a;&c_p;&c_k;&c_a;</trd>
    <trdgrp lng="de">
      <trd>M&uuml;tze</trd>,
      <trd>Kappe</trd>
    </trdgrp>
    <trd lng="en">cap</trd>
    <trd lng="fr">bonnet</trd>
    <trd lng="hu">sapka</trd>
    <trd lng="it">berretto</trd>
    <trd lng="nl">muts</trd>
    <trd lng="pl">czapka</trd>
    <trdgrp lng="pt">
      <trd>barrete</trd>,
      <trd>bon&eacute;</trd>
    </trdgrp>
    <trd lng="ru">&c_sh;&c_a;&c_p;&c_k;&c_a;</trd>
    <trd lng="sk">&ccaron;iapka</trd>
    <trd lng="sv">m&ouml;ssa</trd>
  </snc>
  <snc mrk="cxap.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      Similforma &scirc;irmanta objekto:
      <ekz>
        <tld/>o de skribilo;
      </ekz>
      <ekz>
        <tld/>oj el <klr>[&#x2026;]</klr> fumo super la kamentuboj
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>;
      </ekz>
      <ekz>
        sur iliaj pintoj staras &scirc;ton<tld/>oj <klr>[&#x2026;]</klr> kiel pluvombreloj
        <fnt>
          <bib>Monato</bib>
          <aut>Dimitrije Jani&ccaron;i&cacute;</aut>
          <vrk><url ref="https://www.monato.be/2008/009444.php">Diabla urbeto</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o kiu protektas la pinton de la radiko
        <fnt>
          <vrk><url ref="https://www.jardineriaon.com/eo/partoj-de-la-plantoj.html">Kiuj estas la partoj de plantoj?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">cap</trd>
    <trdgrp lng="fr">
      <trd>capuchon<klr tip="ind"> (fermeture)</klr></trd>,
      <trd>bouchon<klr tip="ind"> (capuchon)</klr></trd>,
      <trd>enjoliveur</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Kappe</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kupak</trd>,
      <trd>sapka</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>cappuccio</trd>,
      <trd>tappo</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>os&lstroke;ona</trd>,
      <trd>ko&lstroke;pak</trd>,
      <trd>pokrywa</trd>,
      <trd>skuwka</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_y;&c_sh;&c_k;&c_a;</trd>,
      <trd>&c_k;&c_o;&c_l;&c_p;&c_a;&c_ch;&c_o;&c_k;</trd>,
      <trd>&c_p;&c_o;&c_k;&c_r;&c_y;&c_sh;&c_k;&c_a;<klr>
      (&c_a;&c_v;&c_t;.)
    </klr></trd>
    </trdgrp>
  </snc>
  <trd lng="af">hoed</trd>
  <trd lng="am">&#x1263;&#x122D;&#x1294;&#x1323;</trd>
  <trd lng="ar">&#x642;&#x628;&#x639;&#x629;</trd>
  <trd lng="az">papaq</trd>
  <trd lng="bn">&#x99F;&#x9C1;&#x9AA;&#x9BF;</trd>
  <trd lng="bo">&#xF5E;&#xFAD;&#xF0B;&#xF58;&#xF7C;&#xF0B;</trd>
  <trd lng="bs">&#x161;e&#x161;ir</trd>
  <trd lng="co">appeddu</trd>
  <trd lng="da">hat</trd>
  <trdgrp lng="es">
    <trd>boina</trd>,
    <trd>tape</trd>,
    <trd>tap&oacute;n</trd>,
    <trd>tapa</trd>,
    <trd>chapa</trd>
  </trdgrp>
  <trd lng="et">m&#xFC;ts</trd>
  <trd lng="fy">hoed</trd>
  <trd lng="gl">sombreiro</trd>
  <trd lng="gu">&#xA9F;&#xACB;&#xAAA;&#xAC0;</trd>
  <trd lng="he">&kaf;&vav;&bet;&ayin;</trd>
  <trd lng="hi">&#x91F;&#x94B;&#x92A;&#x940;</trd>
  <trd lng="hr">&#x161;e&#x161;ir</trd>
  <trd lng="hy">&#x563;&#x56C;&#x56D;&#x561;&#x580;&#x56F;&#x568;</trd>
  <trd lng="is">hatt</trd>
  <trd lng="ja">&#x5E3D;&#x5B50;</trd>
  <trd lng="jw">kupluk</trd>
  <trd lng="ka">&#x10E5;&#x10E3;&#x10D3;&#x10D8;</trd>
  <trd lng="km">&#x1798;&#x17BD;&#x1780;</trd>
  <trd lng="ko">&#xBAA8;&#xC790;</trd>
  <trd lng="lo">&#xEAB;&#xEA5;&#xEB5;&#xE81;</trd>
  <trd lng="ml">&#xD24;&#xD4A;&#xD2A;&#xD4D;&#xD2A;&#xD3F;</trd>
  <trd lng="mr">&#x939;&#x945;&#x91F;</trd>
  <trd lng="my">&#x1026;&#x1038;&#x1011;&#x102F;&#x1015;&#x103A;</trd>
  <trd lng="ne">&#x91F;&#x94B;&#x92A;&#x940;</trd>
  <trd lng="ny">chipewa</trd>
  <trd lng="pa">&#xA1F;&#xA4B;&#xA2A;&#xA40;</trd>
  <trd lng="ps">&#x62E;&#x648;&#x644;&#x6CD;</trd>
  <trd lng="sd">&#x67D;&#x648;&#x67E;&#x64A;</trd>
  <trd lng="si">&#xDAD;&#xDDC;&#xDB4;&#xDCA;&#xDB4;&#xDD2;&#xDBA;</trd>
  <trd lng="sq">hat</trd>
  <trd lng="st">hats</trd>
  <trd lng="ta">&#xBA4;&#xBCA;&#xBAA;&#xBCD;&#xBAA;&#xBBF;</trd>
