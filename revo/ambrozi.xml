<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ambrozi.xml,v 1.8 2023/10/06 06:30:53 revo Exp $">
<kap>
  <rad>Ambrozi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ambrozi.0o">
  <kap><tld/>o</kap>
  <snc mrk="ambrozi.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> grekdevena:
      <ekz>
        &gcirc;uste je la tago de Sankta <tld/>o
        <fnt><bib>SatirRak</bib>, <lok>supersti&ccirc;o</lok></fnt>;
      </ekz>
      <ekz>
        imperiestro Teodozio, devota amiko de la milana arkiepiskopo <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Armando Zecchin</aut>
          <vrk><url ref="&Monato;/2006/008871.php">&Scirc;tato, eklezio kaj ideologia laikeco</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    <ref tip="hom" cel="ambroz.0o">Ambrozio</ref> estas anka&ubreve; dia man&gcirc;a&jcirc;o, de kiu la nomo devenas.
  </rim>
  <trdgrp lng="be">
    <trd>&c_A;&c_m;&c_b;&c_r;&c_o;&c_z;</trd>,
    <trd>&c_A;&c_m;&c_b;&c_r;&c_o;&c_zh;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">Ambrosi</trd>
  <trd lng="de">Ambrosius</trd>
  <trd lng="fr">Ambroise</trd>
  <trd lng="it">Ambrogio</trd>
  <trd lng="pl">Ambro&zdot;y</trd>
</drv>
</art>

