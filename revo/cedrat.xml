<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>cedrat</rad>/o
</kap>
<drv mrk="cedrat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Frukto de <tld/>ujo:
      <ekz>
        kandita <tld/>&scirc;elo, konfititaj <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="ru">&c_c;&c_je;&c_d;&c_r;&c_a;&c_t;</trd>
</drv>
<drv mrk="cedrat.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <dif>
      Speco de citronarbo (<trd lng="la">citrus cetra</trd>).
    </dif>
  </snc>
  <adm>
    Tio estas korektenda. 
  </adm>
</drv>
</art>

