<kap>
  <rad>Klement</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="klement.0o">
  <kap><tld/>o</kap>
  <snc mrk="klement.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        ili kunlaboris kun mi <klr>...</klr> kune kun <tld/>o kaj miaj
        ceteraj kunlaborantoj
        <fnt><bib>NT</bib>, <lok>&Fil; 4:3</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_K;&c_l;&c_ib;&c_m;&c_e;&c_n;&c_t;</trd>
  <trd lng="ca">Climent</trd>
  <trd lng="fr">Cl&eacute;ment</trd>
  <trd lng="it">Clemente</trd>
  <trd lng="pl">Klemens</trd>     
  <trd lng="ro">Clement</trd>
  <trd lng="uk">&#x41A;&#x43B;&c_je;&#x43C;&c_je;&#x43D;&c_t;</trd>
  <trd lng="zh">&#x514B;&#x83B1;&#x95E8; <pr>K&#xE8;l&#xE1;im&#xE9;n</pr></trd>
</drv>
</art>

