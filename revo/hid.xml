<art mrk="$Id: hid.xml,v 1.12 2022/03/20 15:36:34 revo Exp $">
<kap>
  <rad>hid</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="hid.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Timige <ref tip="super" cel="bel.mal0a">malbela</ref>:
      <ekz>
        Levio <klr>...</klr> ridetis tiel malicege, ke lia viza&gcirc;o
        i&gcirc;is tute <tld/>a
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 26a, p. 284a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_r;&c_o;&c_d;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_a;&c_g;&c_ib;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_ch;&c_v;&c_a;&c_r;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ohavn&#xFD;</trd>,
    <trd>ohyzdn&#xFD;</trd>
  </trdgrp>
  <trd lng="en">hideous</trd>
  <trdgrp lng="fr">
    <trd>affreux</trd>,
    <trd>hideux</trd>,
    <trd>immonde</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>irt&oacute;zatos</trd>,
    <trd>iszonyatos</trd>,
    <trd>visszatasz&iacute;t&oacute;</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>orrendo</trd>,
    <trd>ripugnante</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ohydny</trd>,
    <trd>paskudny</trd>
  </trdgrp>
  <trd lng="pt">horrivelmente <ind>feio</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_r;&c_o;&c_d;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_t;&c_v;&c_r;&c_a;&c_t;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ohavn&#xFD;</trd>,
    <trd>ohyzdn&#xFD;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x456;&#x434;&#x440;&#x430;&#x437;&#x43B;&#x438;&#x432;&#x438;&#x439;</trd>,
    <trd>&#x43F;&#x43E;&#x442;&#x432;&#x43E;&#x440;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x441;&#x442;&#x440;&#x430;&#x448;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>
</art>

