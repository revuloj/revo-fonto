<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>grup</rad>/o</kap>
<drv mrk="grup.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Aro da samspecaj objektoj, kunigitaj por sama celo:
      <ekz>
        <tld/>o da
        pentra&jcirc;oj, da dokumentoj, da argumentoj.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Aro da personoj, kiujn io komuna kunigas:
      <ekz>
        <tld/>o da laboristoj, da
        rigardantoj;
      </ekz>
      <ekz>
        <tld/>o da infanoj kune ludantaj;
      </ekz>
      <ekz>
        <tld/>o politika <klr>(da deputitoj samopiniaj)</klr>;
      </ekz>
      <ekz>
        statua <tld/>o;
      </ekz>
      <ekz>
        mar&scirc;i po<tld/>e.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="band.0o">bando</ref>,
      <ref cel="trup.0o">trupo</ref>,
      <ref cel="hord.0o">hordo</ref>,
      <ref cel="parti.0o">partio</ref>.
    </refgrp>
  </snc>
  <snc>
    <uzo tip="fak">FON</uzo>
    <dif>
      Aro da ofte kunigitaj literoj:
      <ekz>
        la <tld/>oj br, kv.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="fak">BIO</uzo>
    <dif>
      Specaro, genro.
    </dif>
  </snc>
  <snc mrk="grup.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 14</lok></fnt>
    <dif>
      Tia <ref tip="super" cel="magm.0o.MAT">magmo</ref> (<em>E</em>,T),
      ke &gcirc;ia <ref cel="operac.0o.MAT">operacio</ref> T estas
      <ref cel="asoci.0eca.MAT">asocieca</ref>,
      ke ekzistas <ref cel="neuxtr.0aelemento">ne&ubreve;tra elemento</ref>
      en <em>E</em>, kaj ke por &ccirc;iu elemento en <em>E</em> ekzistas
      &gcirc;ia <ref cel="neuxtr.0iganto.MAT">ne&ubreve;triganto</ref>:
      <ekz>
        <ind>adicia <tld/>o</ind> <klr>(komuteca <tld/>o, kies operacio estas 
        adicie signata)</klr>
        <fnt><bib>MatVort</bib></fnt>;
        <trd lng="de">additive <ind>Grup</ind></trd>
        <trd lng="en">additive <ind>group</ind></trd>
        <trd lng="fr">groupe additif</trd>
        <trd lng="ru">&c_a;&c_d;&c_d;&c_i;&c_t;&c_i;&c_v;&c_n;&c_a;&c_ja; <ind>&c_g;&c_r;&c_u;&c_p;&c_p;&c_a;</ind></trd>
      </ekz>
      <ekz>
        <tld/>o estas <ref tip="super" cel="monoid.0o">monoido</ref>,
        kies &ccirc;iuj elementoj estas
        <ref cel="neuxtr.0igebla.MAT">ne&ubreve;trigeblaj</ref>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">skupina</trd>
  <trd lng="de">Gruppe</trd>
  <trd lng="en">group</trd>
  <trd lng="es">grupo</trd>
  <trd lng="fr">groupe</trd>
  <trd lng="nl">groep</trd>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_p;&c_p;&c_a;</trd>
</drv>

<drv mrk="grup.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Arigi en <tld/>ojn:
      <ekz>
        <tld/>igi la argumentojn en tri kategoriojn;
      </ekz>
      <ekz>
        la fotografisto <tld/>igas la klientojn;
      </ekz>
      <ekz>
        la tuta cetera esperantistaro <tld/>i&gcirc;is
        &ccirc;irka&ubreve; sia standardo
        <fnt>(<aut>Zamenhof</aut>: <vrk>Parolo
        anta&ubreve; la Kvara Kongreso Esperantista
        en Dresden, 1908</vrk>)</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">gruppieren</trd>
  <trd lng="fr">grouper</trd>
  <trd lng="nl">groeperen</trd>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_p;&c_p;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="grup.0acxo">
  <kap><tld/>a&ccirc;o</kap>
  <snc>
    <dif>
      Malestiminda <tld/>o.
    </dif>
    <refgrp tip="vid">
      <ref cel="koteri.0o">koterio</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Bande</trd>
  <trd lng="fr">bande<klr tip="ind"> (groupe)</klr></trd>
  <trd lng="nl">bende</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_a;&c_j;&c_k;&c_a;</trd>,
    <trd>&c_b;&c_a;&c_n;&c_d;&c_a;</trd>,
    <trd>&c_k;&c_l;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_g;&c_r;&c_u;&c_p;&c_p;&c_i;&c_r;&c_o;&c_v;&c_k;&c_a; <klr>(&c_n;&c_je;&c_o;&c_d;&c_o;&c_b;&c_r;.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="grup.sub0o">
  <kap>sub<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc mrk="grup.sub0o.MAT">
    <dif>
      <klr>(de <tld/>o (<em>E</em>,T))</klr>
      Tia <ref tip="super" cel="grup.0o.MAT"><tld/>o</ref>
      (<em>A</em>,T<sub>A</sub>), ke
      <em>A</em>&#8834;<em>E</em> kaj
      xT<sub>A</sub>y=xTy
      por &ccirc;iuj x, y&#8712;<em>A</em>:
      <ekz>
        nemalplena subaro estas sub<tld/>o nur, se al &gcirc;i apartenas
        rezultoj de la operacio inter kiu ajn unua elemento kaj
        ne&ubreve;triganto de kiu ajn dua.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Untergruppe</trd>
  <trd lng="fr">sous-groupe</trd>
  <trd lng="nl">ondergroep</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_g;&c_r;&c_u;&c_p;&c_p;&c_a;</trd>
</drv>

<drv mrk="grup.alterna0o">
  <kap>alterna <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 14</lok></fnt>
  <snc>
    <dif>
      <klr>(de finia aro <em>E</em>)</klr>
      Sub<tld/>o de ties simetria 
      <ref tip="super" cel="grup.0o.MAT">grupo</ref>, konsistanta el la
      <ref cel="par.0a.permuto">paraj</ref> permutoj.
    </dif>
  </snc>
  <trd lng="de">alternierende <ind>Gruppe</ind></trd>
  <trd lng="en">alternating <ind>group</ind></trd>
  <trd lng="fr">groupe <ind>altern&eacute;</ind></trd>
  <trd lng="ru">&c_z;&c_n;&c_a;&c_k;&c_o;&c_p;&c_je;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_a;&c_ja; <ind>&c_g;&c_r;&c_u;&c_p;&c_p;&c_a;</ind></trd>
</drv>

<drv mrk="grup.arhximeda0o">
  <kap>ar&hcirc;imeda <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      Tia adicia <ref tip="super" cel="grup.0o.MAT">grupo</ref>, provizita
      per
      <ref cel="tut.0eca.rilato">tuteca</ref>
      <ref cel="rilat.ordo0o">ordo-rilato</ref>, ke por &ccirc;iuj du strikte
      <ref cel="poziti.0a.nombro">pozitivaj</ref> elementoj x kaj y en &gcirc;i
      ekzistas tia entjero n, ke n.x&#8805;y:
      <ekz>
        la reeloj konsistigas ar&hcirc;imedan <tld/>on.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="arhxim.0o">Ar&hcirc;imedo</ref>.
    </refgrp>
  </snc>
  <adm>
    Fonto trovenda. <aut>MB</aut>
  </adm>
  <trd lng="de">archimedisch geordnete <ind>Gruppe</ind></trd>
  <trd lng="en">Archimedean <ind>group</ind></trd>
  <trd lng="fr">groupe archim&eacute;dien</trd>
  <trd lng="nl">Archimedische <ind>groep</ind></trd>
  <trd lng="ru">&c_a;&c_r;&c_h;&c_i;&c_m;&c_je;&c_d;&c_o;&c_v;&c_a; <ind>&c_g;&c_r;&c_u;&c_p;&c_p;&c_a;</ind></trd>
</drv>

<drv mrk="grup.fundamenta0o">
  <kap>fundamenta <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(de <ref cel="spac.topologia0o">topologia spaco</ref> &ccirc;e
      punkto a)</klr>
      <ref tip="super" cel="grup.0o.MAT">Grupo</ref> de &ccirc;iuj
      <ref cel="homoto.0a">homotopecaj</ref>
      <ref cel="klas.ekvivalento0o">ekvivalento-klasoj</ref>
      de <ref cel="lacx.0o.MAT">la&ccirc;oj</ref>,
      originantaj &ccirc;e a, provizita per
      <ref cel="lig.kun0o.MAT">kunligo</ref>.
    </dif>
  </snc>
  <ref tip="vid" cel="homoto.0a.irejoj">homotopa.</ref>
  <adm>
    Fonto trovenda. <aut>MB</aut>
  </adm>
  <trd lng="de">Fundamentalgruppe</trd>
  <trd lng="en">fundamental <ind>group</ind></trd>
  <trdgrp lng="fr">
    <trd>groupe fondamental</trd>,
    <trd>groupe de Poincarr&eacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_f;&c_u;&c_n;&c_d;&c_a;&c_m;&c_je;&c_n;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; <ind>&c_g;&c_r;&c_u;&c_p;&c_p;&c_a;</ind></trd>,
    <trd>&c_g;&c_r;&c_u;&c_p;&c_p;&c_a; &c_P;&c_u;&c_a;&c_n;&c_k;&c_a;&c_r;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="grup.galeza0o">
  <kap>galeza <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <subsnc mrk="grup.galeza0o.de_superkorpo">
      <dif>
        <klr>(de <ref cel="korp.super0o">superkorpo</ref> <em>K'</em> de
        <ref cel="korp.0o.MAT">korpo</ref> <em>K</em>)</klr>
        <ref tip="super" cel="grup.0o.MAT"><tld lit="G"/>o</ref> de &ccirc;iuj
        <ref cel="auxtom2.0o">a&ubreve;tomorfioj</ref> de <em>K'</em>, kiuj
        lasas la elementojn de <em>K</em> sen&scirc;an&gcirc;aj:
        <ekz>
          la galezan <tld/>on de superkorpo <em>K'</em> de <em>K</em> oni
          foje signas per Gal(<em>K'</em>/<em>K</em>).
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="grup.galeza0o.de_polinomo">
      <dif>
        <klr>(de <ref cel="polino.0o">polinomo</ref>, a&ubreve;
        algebra <ref cel="ekvaci.0o">ekvacio</ref>)</klr>
        Galeza <tld/>o <sncref ref="grup.galeza0o.de_superkorpo"/> de la
        <ref cel="korp.radika0o">radika korpo</ref> de la polinomo.
      </dif>
    </subsnc>
  </snc>
  <trd lng="de">Galoissche <ind>Gruppe</ind><klr tip="ind"> (von K' &uuml;ber
  K, einer Gleichung)</klr></trd>
  <trd lng="en">Galois['s] <ind>group<klr tip="ind"> (of K' with respect
  to K, of an equation)</klr></ind></trd>
  <trd lng="fr">groupe de Galois<klr tip="ind"> (de K' sur K, d'une
  &eacute;quation)</klr></trd>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_p;&c_p;&c_a; &c_G;&c_a;&c_l;&c_u;&c_a;<klr tip="ind"> (K' &c_n;&c_a;&c_d; K,
  &c_u;&c_r;&c_a;&c_v;&c_n;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>
  <refgrp tip="vid">
    <ref cel="galez.0o">Galezo</ref>.
  </refgrp>
  <adm>
    Fonto trovenda. <aut>MB</aut>
  </adm>
</drv>

<drv mrk="grup.simetria0o">
  <kap>simetria <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 14</lok></fnt>
  <snc>
    <dif>
      <klr>(de finia aro <em>E</em>)</klr>
      <ref tip="super" cel="grup.0o.MAT">Grupo</ref>, konsistanta el la aro
      de &ccirc;iuj <ref cel="bijekc.0o">bijekcioj</ref> super <em>E</em>, 
      provizita per la operacio <ref cel="lig.kun0o.MAT">kunligo</ref>; 
      alidire: <tld/>o de &ccirc;iuj <ref cel="substi.0o.MAT">substituoj</ref>
      super <em>E</em>.
    </dif>
  </snc>
  <trd lng="de">symmetrische <ind>Gruppe</ind></trd>
  <trd lng="en">symmetric <ind>group</ind></trd>
  <trd lng="fr">groupe <ind>sym&eacute;trique</ind></trd>
  <trd lng="ru">&c_s;&c_i;&c_m;&c_je;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja; <ind>&c_g;&c_r;&c_u;&c_p;&c_p;&c_a;</ind></trd>
</drv>
</art>
