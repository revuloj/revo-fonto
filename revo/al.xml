      </ekz>
      <ekz>
        <ind>spite <tld/></ind>;
        <trd lng="hu"><klr>vmi</klr> dac&aacute;ra</trd>
      </ekz>
      <ekz>
        <ind>danke <tld/></ind>.
        <trd lng="hu"><klr>vminek</klr> h&aacute;la</trd>
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>-<ind>nak</ind></trd>,
      <trd>-<ind>nek</ind></trd>
      <trd>-<ind>val</ind></trd>,
      <trd>-<ind>vel</ind></trd>,
    </trdgrp>      
  </snc>
