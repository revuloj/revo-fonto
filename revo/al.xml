<drv mrk="al.0igxilo">
  <kap><tld/>i&gcirc;ilo</kap>
  <snc mrk="al.0igxilo.POL">
    <dif>
      <ref tip="super" cel="formul.0aro.POL">Formularo</ref> por
      <tld/>i&gcirc;i <klr>(plenigita a&ubreve; neplenigita)</klr>:
      <ekz>
        <tld/>i&gcirc;iloj kun promeso tuj komenci la lernadon, kiam dek milionoj da personoj
        estos same promesintaj
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        jam de januaro 1963 la Centra Oficejo de UEA, Roterdamo, povas akcepti vian
        <tld/>i&gcirc;ilon
        <fnt>
          <bib>TkI</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi retenas la kapon sur lia &scirc;ultro rigardante kiel li skribas mian nomon sur la
        <tld/>i&gcirc;ilon
        <fnt>
          <vrk><url ref="Spomenka &Scaron;timec">Ombro sur interna pejza&gcirc;o</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Anmeldeformular</trd>,
    <trd>Beitrittserkl&auml;rung</trd>
