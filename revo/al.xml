<subart>
  <dif>
    Prefikso kun la sencoj
    <tld/><sncref ref="al.0.prep_proksimigxo"/>,
    <tld/><sncref ref="al.0.prep_celo"/>,
    <tld/><sncref ref="al.0.prep_koncerno"/>,
    <tld/><sncref ref="al.0.prep_de"/>:
  </dif>
  <snc ref="al.0.prep_proksimigxo">    
    <ekz>
      <ref cel="ven.al0i"><tld/>veni</ref>,
      <ref cel="kur.al0i"><tld/>kuri</ref>, 
      <ref cel="port.al0i"><tld/>porti</ref>, 
      <ref cel="est.al0i"><tld/>esti</ref>;
    </ekz>
  </snc>
  <snc ref="al.0.prep_celo">
    <ekz>
      <tld/>venigi<fnt><bib>MT</bib><lok>Eliro 29:4</lok></fnt>;
    </ekz>
    <ekz>
      <ref cel="konduk.al0i"><tld/>konduki</ref>;
    </ekz>
    <ekz>
      <ind><tld/>rigardi</ind>
      <fnt>
        <bib>NT</bib>
        <lok>Luko 22:61</lok>
      </fnt>;
      <trd lng="hu">odan&eacute;z</trd>
      <trd lng="nl">aankijken</trd>
    </ekz>
  </snc>
  <snc ref="al.0.prep_koncerno">
    <ekz>
      <ind><tld/>proprigi</ind>
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 36:5</lok>
        </fnt>;
      <trd lng="hu">elsaj&aacute;t&iacute;t</trd>
      <trd lng="nl">toe-eigenen</trd>
    </ekz>
    <ekz>
      <ref cel="mezur.al0i"><tld/>mezuri</ref>,
      <ref cel="far.al0i"><tld/>fari</ref>,
      <ref cel="gxust.al0igi"><tld/>&gcirc;ustigi</ref>;
    </ekz>
  </snc>
  <snc ref="al.0.prep_de">
    <ekz>
      <ref cel="don.al0i"><tld/>doni</ref> <klr>(doni plie, adicii)</klr>;
    </ekz>
    <ekz>
      <ref cel="dir.al0i"><tld/>diri</ref> <klr>(diri plie)</klr>;
    </ekz>
    <ekz>
      <ref cel="met.al0i"><tld/>meti</ref>;
    </ekz>
    <ekz>
      la neesprimita subjekto estas facile <tld/>pensebla.
    </ekz>
  </snc>
</subart>
  <trdgrp lng="hu">
