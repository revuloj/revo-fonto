  <trd lng="ja">&#23455;&#29694;&#12377;&#12427;  [<ind>&#12376;&#12388;&#12370;&#12435;&#12377;&#12427;</ind>]</trd>
  <trd lng="uk">&c_z;&c_d;&c_ib;&c_j;&c_s;&c_n;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
