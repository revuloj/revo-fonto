<drv mrk="al.mal0igxi">
  <kap>
    mal<tld/>i&gcirc;i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="al.mal0igxi.KOMUNE">
    <dif>
      &Ccirc;esigi, <ref tip="vid" cel="rezign.0i">rezigni</ref>
      intencitan a&ubreve; efektivan <tld/>i&gcirc;on:
      <ekz>
        ja ekzistis reguloj de internacia juro por forlasi la traktatojn de la Unio, tio
        estas, oni jam povis mal<tld/>i&gcirc;i el la E&ubreve;ropa Unio senprobleme
        <fnt>
          <aut>P. Foche</aut>
          <vrk><url ref="https://satesperanto.org/spip.php?article248">Argumentoj kontra&ubreve; la
          Konstitucia Traktato</url></vrk>
          <lok>el SAT-diskuto, 2005-03-23</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>sich <ind>abmelden</ind></trd>,
    <trd>austreten</trd>,
    <trd>ausscheiden</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>d&eacute;sinscrire</ind></trd>,
    <trd>se <ind>retirer</ind> de</trd>
  </trdgrp>
</drv>
