  <gra><vspec>ntr</vspec></gra>
  <snc mrk="al.0igxi.KOMUNE">
    <dif>
      I&gcirc;i parto, membro (<ref tip="sub" cel="an.0igxi">ani&gcirc;i</ref>),
      akompananto a&ubreve; partoprenanto de io:
      <ekz>
        beda&ubreve;rinde, ke <tld/>i&gcirc;is al la kongreso nur 150 kongresontoj
        <fnt>
          <bib>LOdE</bib>
          <aut>Wolfgang Kirschstein</aut>
          <vrk><url ref="&Tekstaro;?nomo=ondo-de-esperanto">Frazoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;i al la kristandemokrata partio
        <fnt>
          <aut>Guido van Damme</aut>
          <vrk><url ref="&Monato;/2001/007259.php">Demokratio blokita</url></vrk>
          <bib>Monato</bib>
          <lok>2001/01, p. 8</lok>
          </fnt>.
      </ekz>
      <ekz>
        post iom da tempo al li <tld/>i&gcirc;is junulo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Feli&ccirc;a Jo&ccirc;jo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>se gut' <tld/> guto <tld/>i&gcirc;as
        <klr>(kuni&gcirc;as)</klr>, maro
        fari&gcirc;as</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">cseppb&odblac;l lesz a tenger</trd>
      </ekz>
      <ekz>
        al tio &ccirc;i <tld/>i&gcirc;as multego da deklinacioj neregulaj
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        al lia anta&ubreve;a embaraso <tld/>i&gcirc;is nun iom da konfuzo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        al la koreco <tld/>i&gcirc;adis <klr>[&#8230;]</klr> videbla nuanco
        de silenta kompato
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
