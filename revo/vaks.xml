<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vaks.xml,v 1.27 2022/05/06 12:17:21 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vaks</rad>/o
</kap>
<drv mrk="vaks.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Flava knedebla <ref tip="super" cel="substa.0o.FIZ">substanco</ref>, 
      el kiu <ref cel="abel.0o">abelo</ref>j konstruas siajn &ccirc;elojn:
      <ekz>
        pro viaj komerca&jcirc;oj ili alportadis al vi <klr>[&#x2026;]</klr> <tld/>on, mielon, oleon
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 27:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        tien flugis la abelre&gcirc;ino kun sia svarmo kaj demetis <tld/>on kaj mielon
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de la jaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la potenca <nom>Herhor</nom> pali&gcirc;is kiel <tld/>o kaj ne povis eligi vo&ccirc;on el la kunpremita
        laringo
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tuta horizonto ekflami&gcirc;is per ru&gcirc;a fajro kaj montoj kaj arbaroj fandi&gcirc;is kiel
        <tld/>o en forno
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia koro fari&gcirc;is kiel <tld/>o, fandi&gcirc;is en mia interno
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 22:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[karaktero]</klr> mola kiel <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_o;&c_s;&c_k;</trd>
    <trd lng="bg">&c_v;&c_o;&c_s;&c_malmol;&c_k;</trd>
    <trd lng="pt">cera de abelha</trd>
    <trd lng="ru">&c_v;&c_o;&c_s;&c_k;</trd>
  </snc>
  <snc>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Iu miksa&jcirc;o el diversspecaj hidrokarbonoj kun similaj ecoj: solida je 20&Celsius;,
      likvi&gcirc;anta je &ccirc;irka&ubreve; 40&Celsius; eventuale iom diafana kaj ekbrilanta sub modesta premo:
      <ekz>
        apud la muro inter la fenestroj staris kanapo, kovrita per nigra sen&scirc;eligita
        <tld/>tolo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        li nun iris tra la resonantaj koridoroj, en kiuj odoris planko<tld/>o
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Sen&ccirc;esa Rakonto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel moliga&jcirc;o uzi&gcirc;as <tld/>oj, ekzemple karna&ubreve;ba (palmolea) <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2014/011143.php">Plasto planta</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        la produktopa&scirc;oj &ccirc;efe konsistis el dispecigo de la skisto, kribrado, seka
        distilado, eltiro de gudro, distilado de kruda oleo kaj distilado de kruda
        <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Wu Guojiang</aut>
          <vrk><url ref="&Monato;/2008/009548.php">Rafinejo jubileas</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="cir.0o">ciro</ref>,
      <ref cel="lipid.0o">lipido</ref>,
      <ref cel="parafi.0o">parafino</ref>,
      <ref cel="seb.0o">sebo</ref>,
      <ref cel="vazeli.0o">vazelino</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_v;&c_o;&c_s;&c_k;</trd>,
      <trd>&c_m;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_k;&c_a;</trd>,
      <trd>&c_v;&c_a;&c_k;&c_s;&c_a;</trd>
    </trdgrp>
    <trd lng="pt">cera</trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_o;&c_s;&c_k;</trd>,
      <trd>&c_m;&c_a;&c_s;&c_t;&c_i;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <trd lng="br">koar</trd>
  <trd lng="ca">cera</trd>
  <trd lng="cs">vosk</trd>
  <trd lng="de">Wachs</trd>
  <trd lng="en">wax</trd>
  <trd lng="es">cera</trd>
  <trd lng="fr">cire</trd>
  <trd lng="hu">viasz</trd>
  <trd lng="it">cera</trd>
  <trd lng="nl">was</trd>
  <trd lng="sk">vosk</trd>
  <trdgrp lng="uk">
    <trd>&#x432;&#x456;&#x441;&#x43A;</trd>,
    <trd>&#x43C;&#x430;&#x441;&#x442;&#x438;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>

<drv mrk="vaks.0i">
  <kap><tld/>i, <var><kap><tld/>umi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;miri per <tld/>o:
      <ekz>
        la malgrandajn <klr>[kandelajn stumpetojn]</klr> la maljuna virino 
        uzis por <tld/>i per ili sian fadenon &ccirc;e la kudrado
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova strata lanterno</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili dormadis sur molaj kusenoj, kuradis sur <tld/>itaj pargetoj, man&gcirc;adis
        bonegajn delikata&jcirc;ojn
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> <tld/>umita planko brilis kiel spegulo
        <fnt>
          <bib>KPr</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_c;&c_ib;&c_r;&c_a;&c_c;&c_mol;
      <ind>&c_v;&c_o;&c_s;&c_k;&c_a;&c_m;</ind></trd>,
    <trd>&c_v;&c_a;&c_sh;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_a;&c_s;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_a;&c_k;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">koara&ntilde;</trd>
  <trd lng="ca">encerar</trd>
  <trd lng="cs">voskovat</trd>
  <trdgrp lng="de">
    <trd>wachsen</trd>,
    <trd>mit Wachs <ind>einschmieren</ind></trd>,
    <trd>wienern</trd>
  </trdgrp>
  <trd lng="en">wax</trd>
  <trd lng="es">encerar</trd>
  <trd lng="fr">cirer</trd>
  <trdgrp lng="hu">
    <trd>viaszol</trd>,
    <trd>viaszoz</trd>,
    <trd>vakszol</trd>,
    <trd>vikszel</trd>
  </trdgrp>
  <trd lng="it">incerare</trd>
  <trdgrp lng="nl">
    <trd>wassen<klr tip="ind"> (inwrijven)</klr></trd>,
    <trd>inwrijven<klr tip="ind"> (met was)</klr></trd>,
    <trd>insmeren<klr tip="ind"> (met was)</klr></trd>
  </trdgrp>
  <trd lng="pt">encerar</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_t;&c_i;&c_r;&c_a;&c_t;&c_mol; 
<ind>&c_v;&c_o;&c_s;&c_k;&c_o;&c_m;</ind></trd>,
    <trd>&c_v;&c_o;&c_shch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">voskova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x432;&#x43E;&#x441;&#x43A;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x43E;&#x449;&#x438;&#x442;&#x438;</trd>,
    <trd>&#x43D;&#x430;&#x442;&#x438;&#x440;&#x430;&#x442;&#x438; &#x432;&#x43E;&#x441;&#x43A;&#x43E;&#x43C;</trd>,
    <trd>&#x43C;&#x430;&#x441;&#x442;&#x438;&#x43A;&#x43E;&#x44E;</trd>
  </trdgrp>
</drv>

<drv mrk="vaks.sigel0o">
  <kap><ofc>*</ofc>sigel<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="V"/>o (a&ubreve; <ref cel="lak.0o">lako</ref>) uzata por <ref cel="sigel.0i.TIP">sigeli</ref>:
      <ekz>
        en la kavon de la pladeto oni nun fandas sigel<tld/>on, kiu tute kovras la
        &scirc;nuron, kaj poste oni premas sur &gcirc;in la amerikan sigelon
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la kuiristino alfaris al <klr>[la flikilo]</klr> kapon el sigel<tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Flikilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kesto ricevis la koloron de ru&gcirc;a sigel<tld/>o
        <fnt>
          <bib>ViV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="sxelak.0o">&scirc;elako</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_r;&c_g;&c_u;&c_ch;</trd>
  <trd lng="br">koar siella&ntilde;</trd>
  <trd lng="ca">lacre</trd>
  <trd lng="cs">pe&ccaron;etn&iacute; vosk</trd>
  <trd lng="de">Siegelwachs</trd>
  <trd lng="fr">cire &agrave; cacheter</trd>
  <trd lng="hu">pecs&eacute;tviasz</trd>
  <trd lng="it">ceralacca</trd>
  <trd lng="nl">zegellak</trd>
  <trd lng="pt">lacre<klr tip="amb"> (de cera)</klr></trd>
  <trd lng="ru">&c_s;&c_u;&c_r;&c_g;&c_u;&c_ch;</trd>
  <trd lng="sk">pe&ccaron;atn&yacute; vosk</trd>
  <trd lng="uk">&#x441;&#x443;&#x440;&#x433;&#x443;&#x447;</trd>
</drv>
</art>
