<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vaks</rad>/o
</kap>
<drv mrk="vaks.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Flava knedebla substanco, el kiu abeloj konstruas siajn &ccirc;elojn:
      <ekz>
        karaktero mola kiel <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a (palflava) viza&gcirc;o<fnt>B</fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_v;&c_o;&c_s;&c_k;</trd>
  </snc>
  <snc>
    <dif>
      Analogaj substancoj tirataj el iaj vegeta&jcirc;oj a&ubreve; &hcirc;emie fabrikataj:
      <ekz>
        <tld/>tapi&scirc;o
        <klr>(linoleumo)</klr>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_v;&c_o;&c_s;&c_k;</trd>,
      <trd>&c_m;&c_a;&c_s;&c_t;&c_i;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <trd lng="de">Wachs</trd>
  <trd lng="es">cera</trd>
  <trdgrp lng="fr">
    <trd>cire</trd>
  </trdgrp>
  <trd lng="nl">was</trd>
</drv>

<drv mrk="vaks.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;miri per <tld/>o:
      <ekz>
        <tld/>i meblon, plankon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>wachsen</trd>,
    <trd>mit Wachs <ind>einschmieren</ind></trd>
  </trdgrp>
  <trd lng="es">encerar</trd>
  <trdgrp lng="fr">
    <trd>cirer</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>wassen<klr tip="ind"> (inwrijven)</klr></trd>,
    <trd>inwrijven<klr tip="ind"> (met was)</klr></trd>,
    <trd>insmeren<klr tip="ind"> (met was)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_t;&c_i;&c_r;&c_a;&c_t;&c_mol; <ind>&c_v;&c_o;&c_s;&c_k;&c_o;&c_m;</ind></trd>,
    <trd>&c_v;&c_o;&c_shch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="vaks.sigel0o">
  <kap><ofc>*</ofc>sigel<tld/>o</kap>
  <snc>
    <dif>
      Speco de <tld/>o uzata por sigeli.
    </dif>
  </snc>
  <trd lng="de">Siegelwachs</trd>
  <trdgrp lng="fr">
    <trd>cire &agrave; cacheter</trd>
  </trdgrp>
  <trd lng="nl">zegellak</trd>
  <trd lng="ru">&c_s;&c_u;&c_r;&c_g;&c_u;&c_ch;</trd>
</drv>
</art>
