<art mrk="$Id: oklahom.xml,v 1.7 2023/09/03 07:12:33 revo Exp $">
<kap>
  <rad>Oklahom</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="oklahom.0o">
  <kap><tld/>o</kap>
  <snc mrk="oklahom.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Sub&scirc;tato en la centra parto de
      <ref tip="malprt" cel="uson.U0o.GEOG">Usono</ref>:
      <ekz>
        la formo de tri el la usonaj sub&scirc;tatoj &mdash; Teksaso, 
        <tld/>o, kaj Florido &mdash; havas malvastan protruda&jcirc;on; 
        tion ni nomas &leftquot;patotenilo&rightquot;
        <fnt>
          <aut>K. Miner</aut>,
          <vrk><url ref="http://www.sunflower.com/~miner/TAGLIBRO_package/Novembro_2004.html">
          taglibro</url></vrk>,
          <lok>2004-11-11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_A;&c_k;&c_l;&c_a;&c_g;&c_o;&c_m;&c_a;</trd>
  <trd lng="cs">Oklahoma</trd>
  <trd lng="en">Oklahoma</trd>
  <trd lng="fr">Oklahoma</trd>
  <trd lng="ja">&#x30AA;&#x30AF;&#x30E9;&#x30DB;&#x30DE;&#x5DDE;  [<ind>&#x30AA;&#x30AF;&#x30E9;&#x30DB;&#x30DE;&#x3057;&#x3085;&#x3046;</ind>]</trd>
  <trd lng="pl">Oklahoma</trd>
  <trd lng="ro">Oklahoma</trd>
  <trd lng="sk">Oklahoma</trd>
  <trd lng="uk">&#x41E;&#x43A;&#x43B;&c_a;&c_h;&#x43E;&#x43C;&c_a; (&#x43E;&c_d;&c_i;&#x43D; &c_ib;&c_z;
  &c_a;&#x43C;&c_je;&c_r;&c_i;&#x43A;&c_a;&#x43D;&c_s;&#x44C;&#x43A;&c_i;&c_h; &c_sh;&c_t;&c_a;&c_t;&c_ib;&c_v;)</trd>
</drv>
</art>

