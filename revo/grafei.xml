<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>grafeik</rad>/o
</kap>
<drv mrk="grafei.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt>
    <bib>KompLeks</bib> 
    <lok><url ref="&KompLeks;/G.html#GRAFEIKO">TTT</url></lok>
  </fnt>
  <snc>
    <dif>
      Bran&ccirc;o de <ref tip="malprt" cel="matema.0o">matematiko</ref>,
      studanta <ref tip="prt" cel="grafe.0o">grafeojn</ref>:
      <ekz>
        la <ind>problemo pri la K&ouml;nigsberg-aj pontoj</ind> estas klasika
        problemo de <tld/>o;
        <trd lng="de">Problem der K&ouml;nigsberger Br&uuml;cken</trd>
        <trd lng="en">K&ouml;nigsberg bridge problem</trd>
        <trd lng="fr">probl&egrave;me des ponts de K&ouml;nigsberg</trd>
        <trd lng="hu">a <ind>k&ouml;nigsbergi</ind> hidak probl&eacute;m&aacute;ja</trd>
        <trd lng="pl">zagadnienie most&oacute;w kr&oacute;lewieckich</trd>
        <trd lng="ru">&c_z;&c_a;&c_d;&c_a;&c_ch;&c_a; &c_o; &c_k;&c_jo;&c_n;&c_i;&c_g;&c_s;&c_b;&c_je;&c_r;&c_g;&c_s;&c_k;&c_i;&c_h; &c_m;&c_o;&c_s;&c_t;&c_a;&c_h;</trd>
      </ekz>
      <ekz>
        la solvo evidentas el la <tld/>aj konsideroj.
      </ekz>
    </dif>
    <ref tip="sin" cel="grafe.0teorio">grafeteorio.</ref>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_o;&c_r;&c_y;&c_ja; &c_g;&c_r;&c_a;&c_f;&c_a;&c_w;</trd>
  <trd lng="de">Graphentheorie</trd>
  <trd lng="en">graph theory</trd>
  <trd lng="es">teor&iacute;a de grafos</trd>
  <trd lng="fr">th&eacute;orie des graphes</trd>
  <trd lng="hu">gr&aacute;felm&eacute;let</trd>
  <trd lng="nl">grafentheorie</trd>
  <trd lng="pl">teoria graf&oacute;w</trd>
  <trd lng="ru">&c_t;&c_je;&c_o;&c_r;&c_i;&c_ja;
&c_g;&c_r;&c_a;&c_f;&c_o;&c_v;</trd>
</drv>
</art>

