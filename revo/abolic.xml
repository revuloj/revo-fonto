<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: abolic.xml,v 1.28 2022/02/20 20:28:17 revo Exp $">
<kap>
  <rad>abolici</rad>/i
</kap>
<drv mrk="abolic.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="abolic.0i.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      <ref tip="dif" cel="abol.0i.JUR">Aboli</ref>:
      <ekz>
        sklaveco estis <tld/>ita en Usono en 1865.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_m;&c_ja;&c_n;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">terri&ntilde;<klr tip="ind"> (ul lezenn)</klr></trd>
  <trdgrp lng="ca">
    <trd>abolir</trd>,
    <trd>revocar</trd>,
    <trd>abrogar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>vym&yacute;tit zvyk</trd>,
    <trd>zastavit soudn&iacute; &rcaron;&iacute;zen&iacute;</trd>,
    <trd>zru&scaron;it z&aacute;kon</trd>
  </trdgrp>
  <trdgrp lng="da">
    <trd>afskaffe</trd>,
    <trd>oph&aelig;ve</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>aufheben</trd>,
    <trd>abschaffen</trd>,
    <trd>einstellen</trd>
  </trdgrp>
  <trd lng="en">abolish</trd>
  <trd lng="es">abolir</trd>
  <trdgrp lng="fa">
    <trd>&a_l;&a_gh;&a_w; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_b;&a_r;&f_ch;&f_y;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>abolir</trd>,
    <trd>abroger</trd>,
    <trd>annuler<klr tip="ind"> (par un acte juridique)</klr></trd>
  </trdgrp>
  <trd lng="he">&lamed;&bet;&tet;&lamed;</trd>
  <trd lng="hu">elt&ouml;r&ouml;l</trd>
  <trdgrp lng="it">
    <trd>abolire</trd>,
    <trd>abrogare</trd>
  </trdgrp>
  <trd lng="nl">afschaffen</trd>
  <trd lng="oc">abolir</trd>
  <trdgrp lng="pl">
    <trd>umorzy&cacute;</trd>,
    <trd>uchyli&cacute;</trd>,
    <trd>znie&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>abolir</trd>,
    <trd>anular</trd>,
    <trd>extinguir</trd>,
    <trd>revogar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_m;&c_je;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_p;&c_r;&c_a;&c_z;&c_d;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dekr&eacute;t</trd>,
    <trd>zru&scaron;i&tcaron; <klr>(z&aacute;kon, oby&ccaron;aj)</klr></trd>,
    <trd>zru&scaron;i&tcaron; z&aacute;kon</trd>
  </trdgrp>
</drv>
</art>
