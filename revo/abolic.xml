<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>abolici</rad>/i
</kap>
<drv mrk="abolic.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Per jura akto definitive nuligi le&gcirc;on a&ubreve;
      forigi kutimon:
      <ekz>
        sklaveco estis <tld/>ita en Usono nur en 1865.
      </ekz>
    </dif>
    <ref tip="sin" cel="abol.0i">aboli</ref>
    <ref tip="vid" cel="nul0.0igi">nuligi</ref>
  </snc>
  <trd lng="ca">abolir</trd>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_m;&c_ja;&c_n;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">terri&ntilde;<klr tip="ind"> (ul lezenn)</klr></trd>
  <trdgrp lng="de">
    <trd>aufheben</trd>,
    <trd>abschaffen</trd>,
    <trd>einstellen</trd>
  </trdgrp>
  <trd lng="en">abolish</trd>
  <trd lng="es">abolir</trd>
  <trdgrp lng="fa">
    <trd>&a_l;&a_gh;&a_w; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_b;&a_r;&f_ch;&f_y;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>abolir</trd>,
    <trd>abroger</trd>,
    <trd>annuler<klr tip="ind"> (par un acte juridique)</klr></trd>
  </trdgrp>
  <trd lng="hu">elt&ouml;r&ouml;l</trd>
  <trd lng="it">abrogare</trd>
  <trd lng="nl">afschaffen</trd>
  <trd lng="oc">abolir</trd>
  <trdgrp lng="pl">
    <trd>umorzy&cacute;</trd>,
    <trd>uchyli&cacute;</trd>,
    <trd>znie&sacute;&cacute;</trd>
  </trdgrp>
  <trd lng="pt">abolir, anular, extinguir, revogar</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_m;&c_je;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_p;&c_r;&c_a;&c_z;&c_d;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zru&scaron;i&tcaron; <klr>(z&aacute;kon,
oby&ccaron;aj)</klr></trd>
</drv>
</art>
