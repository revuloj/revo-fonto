<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Saturn</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="saturn.0o">
  <kap><tld/>o</kap>
  <snc mrk="saturn.0o.mita">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Romia <ref tip="super" cel="di.0o.politeisma">dio</ref>
      de terkulturado, analogo de helena
      <ref tip="vid" cel="kron1.0o">Krono</ref>.
    </dif>
    <trd lng="hu">Saturnus</trd>
  </snc>
  <snc mrk="saturn.0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      La sesa (la&ubreve; distanco de la <ref tip="vid"
      cel="sun.0o.nia">Suno</ref>) kaj la dua la&ubreve; grandeco
      <ref tip="super" cel="planed.0o">planedo</ref>
      de la <ref tip="malprt" cel="sun.0sistemo">sunsistemo</ref>,
      fama pro siaj rimarkeblaj ringoj.
    </dif>
    <trd lng="hu">Szaturnusz</trd>
    <trd lng="zh">&#22303;&#26143;<klr> [T&#468;x&#299;ng]</klr></trd>
  </snc>
  <trd lng="be">&c_S;&c_a;&c_t;&c_u;&c_r;&c_n;</trd>
  <trd lng="de">Saturn</trd>
  <trd
lng="el">&Kappa;&rho;&omikron_acute;&ny;&omikron;&sigma_fina;</trd>
  <trd lng="en">Saturn</trd>
  <trd lng="es">Saturno</trd>
  <trd lng="pt">Saturno</trd>
  <trd lng="fr">Saturne</trd>
  <trd lng="it">Saturno</trd>
  <trd lng="nl">Saturnus</trd>
  <trd lng="pl">Saturn</trd>
  <trd lng="ru">&c_S;&c_a;&c_t;&c_u;&c_r;&c_n;</trd>
</drv>
</art>
