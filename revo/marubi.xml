<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: marubi.xml,v 1.3 2023/11/03 12:47:03 afido Exp $">
    <kap>
      <rad>marubi</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="marubi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="marubi.0o.genro">
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref> el
      familio <ref tip="malprt" cel="lamiac.0oj">lamiacoj</ref> (<trd lng="la">Marrubium</trd>),
      de plej ofte plurjaroj herboj kun zigomorfaj kvinnombraj blankaj a&ubreve; ru&gcirc;etaj floretoj, ofte
      feltaj folioj,
      komunaj en E&ubreve;razio:
      <ekz>
        <tld/>oj <klr>[&#8230;]</klr> estis man&gcirc;ataj de Judoj dum Pesa&hcirc;o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=355811"
              >Marubio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj dropsoj kun ekstrakto de ordinara <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=692894"
              >Dropso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/8/82/Marrubium_astracanicum_1.jpg/288px-Marrubium_astracanicum_1.jpg"
      prm="CC BY-SA 3.0"
      ><tld/>o (Marrubium astracanicum), Turkujo
        <fnt>
          <aut>Franz Xaver, propra verko</aut>
          <lok><url ref="&WCW;/File:Marrubium_astracanicum_1.jpg"
              >https://commons.wikimedia.org/wiki/File:Marrubium_astracanicum_1.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="be">&c_sh;&c_a;&c_n;&c_t;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Andorn</trd>,
      <trd>Orant</trd>,
      <trd>Mutterkraut</trd>
    </trdgrp>
    <trd lng="en">horehound</trd>
    <trd lng="fr">Marrubium</trd>
  </snc>
</drv>

  </art>

