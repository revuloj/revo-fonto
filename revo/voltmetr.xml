<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: voltmetr.xml,v 1.2 2023/05/06 15:22:44 afido Exp $">
<kap>
  <rad>voltmetr</rad>/o
  <fnt><bib>PIV2</bib></fnt>
</kap>

<drv mrk="voltmetr.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">ELE</uzo>
    <fnt><bib>PV</bib></fnt>
    <dif>
      Instrumento por mezuri <ref tip="super" cel="tensi.0o.ELE">tension</ref>:
      <ekz>
        oni mezuras la potencialon kiel la elektran tension <klr>[per]</klr> <tld/>o
        <klr>[a&ubreve;]</klr> osciloskopo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=290396"
              >Elektra potencialo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        al la dekstra bobeno (induktobobeno) estas konektita <tld/>o, kiu mezuras la
        produktatan tensio
        <fnt>
          <aut>Walter Bernard</aut>
          <vrk><url ref="http://www.eventoj.hu/steb/fiziko/fiziklibro/2_fiziklibro.pdf"
              >Fiziko - Baza Kurso, Dua Parto</url></vrk>
          <lok>p.53</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="amperm.0o">ampermetro</ref>,
      <ref cel="om.o0metro">omometro</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Spannungsmessger&auml;t</trd>,
      <trd>Voltmeter</trd>
    </trdgrp>
    <trd lng="en">voltmeter</trd>
    <trd lng="fr"> voltm&egrave;tre</trd>
  </snc>
</drv>

</art>

