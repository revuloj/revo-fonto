<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: perl.xml,v 1.29 2024/02/17 06:43:06 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>perl</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="perl.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="perl.0o.JUV_konka">
     <dif>
       Brilblanka globeto formi&gcirc;anta en kelkaj specoj de markonkoj
       kaj uzata kiel juvelo:
       <ekz>
         la virino estis <klr>...</klr> ornamita
         per oro kaj altvaloraj &scirc;tonoj kaj <tld/>oj
         <fnt><bib>NT</bib>, <lok>&Apo; 17:4</lok></fnt>.
       </ekz>
     </dif>
     <trd lng="be">&c_zh;&c_a;&c_m;&c_ch;&c_u;&c_zh;&c_y;&c_n;&c_a;</trd>
     <trdgrp lng="ru">
       <trd>&c_zh;&c_je;&c_m;&c_ch;&c_u;&c_g;</trd>,
       <trd>&c_zh;&c_je;&c_m;&c_ch;&c_u;&c_zh;&c_i;&c_n;&c_a;</trd>,
       <trd>&c_p;&c_je;&c_r;&c_l;</trd>
     </trdgrp>
   </snc>
   <snc mrk="perl.0o.JUV_bida">
     <dif>
       Vitra, emajla a&ubreve; metala globeto, kiun oni traboras kaj
       surfadenigas por fari diversajn ornama&jcirc;ojn;
       globa <ref tip="super" cel="bid.0o.JUV">bido</ref>.
     </dif>
     <trdgrp lng="be">
       <trd>&c_b;&c_ib;&c_s;&c_e;&c_r;&c_y;&c_n;&c_a;</trd>,
       <trd>&c_p;&c_a;&c_c;&c_je;&c_r;&c_a;&c_ch;&c_k;&c_a;</trd>
     </trdgrp>
     <trd lng="ru">&c_b;&c_i;&c_s;&c_je;&c_r;</trd>
   </snc>
   <snc mrk="perl.0o.FIG">
     <uzo tip="stl">FIG</uzo>
     <dif>
       Altvalora&jcirc;o, perfekta&jcirc;o, plej bela a&jcirc;o:
       <ekz>
         tio estas la <tld/>o el la kolekto;
       </ekz>
       <ekz>
         li estas la <tld/>o el la edzoj;
       </ekz>
       <ekz>
         literatura <tld/>o;
       </ekz>
       <ekz>
         ne donu sankta&jcirc;on al la hundoj, nek &jcirc;etu viajn
         <tld/>ojn anta&ubreve; la porkoj
         <fnt><bib>NT</bib>, <lok>&Mat; 7:6</lok></fnt>.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="gem.0o.FIG">gemo<sncref/></ref>,
       <ref cel="krem.0o.plejbonajxo">kremo<sncref/></ref>,
       <ref cel="trezor.0o.aprezindajxo">trezoro<sncref/></ref>.
     </refgrp>
     <trd lng="be">&c_p;&c_e;&c_r;&c_l;&c_a;</trd>
     <trd lng="ru">&c_p;&c_je;&c_r;&c_l;</trd>
   </snc>
  <trd lng="bo">&#xF55;&#xFB2;&#xF7A;&#xF44;&#xF0B;&#xF62;&#xFA1;&#xF7C;&#xF42;&#xF0B;</trd>
  <trd lng="br">perlezenn</trd>
  <trd lng="ca">perla</trd>
  <trd lng="cs">perla</trd>
  <trd lng="de">Perle</trd>
  <trd lng="en">pearl</trd>
  <trd lng="es">perla</trd>
   <trd lng="fr">perle</trd>
  <trdgrp lng="hu">
     <trd>gy&ouml;ngy</trd>,
     <trd>gy&ouml;ngyszem</trd>
   </trdgrp>
  <trd lng="id">mutiara</trd>
  <trdgrp lng="ja">
    <trd>&#x771F;&#x73E0;  <pr>&#x3057;&#x3093;&#x3058;&#x3085;</pr></trd>,
    <trd>&#x8CB4;&#x91CD;&#x306A;&#x3082;&#x306E;  <pr>&#x304D;&#x3061;&#x3087;&#x3046;&#x306A;&#x3082;&#x306E;</pr></trd>,
    <trd>&#x7CBE;&#x83EF;  <pr>&#x305B;&#x3044;&#x304B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
     <trd>parel</trd>,
     <trd>kraal</trd>
   </trdgrp>
  <trd lng="pl">per&lstroke;a</trd>
  <trd lng="pt">p&eacute;rola</trd>
  <trd lng="sk">perla</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_je;&c_r;&#x43B;&c_i;&#x43D;&c_a;</trd>,
    <trd>&#x43F;&c_je;&c_r;&#x43B; (&#x43F;&c_r;&#x43E; &#x43C;&c_i;&c_s;&c_t;&c_je;&c_c;&#x44C;&#x43A;&c_i;&c_j; &c_t;&c_v;&c_ib;&c_r;)</trd>,
    <trd>&#x43D;&c_a;&#x43C;&c_i;&c_s;&c_t;&c_i;&#x43D;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x73E0; <pr>zh&#x16B;</pr></trd>,
    <trd>&#x73E0;&#x5B50; <pr>zh&#x16B;zi</pr></trd>,
    <trd>&#x73CD;&#x73E0; <pr>zh&#x113;nzh&#x16B;</pr></trd>,
    <trd>&#x74B8; <pr>b&#x12B;n</pr></trd>,
    <trd>&#x660E;&#x73E0; <pr>m&#xED;ngzh&#x16B;</pr></trd>,
    <trd>&#x73AD; <pr>p&#xED;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="perl.0a">
   <kap><tld/>a</kap>
   <snc mrk="perl.0a.KOMUNE">
     <dif>
       Simila al <tld/>o, <tld/>okolora:
       <ekz>
         <tld/>aj dentoj;
       </ekz>
       <ekz>
         <tld/>a &ccirc;ielo, roso<fnt>B</fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_zh;&c_a;&c_m;&c_ch;&c_u;&c_zh;&c_n;&c_y;</trd>
  <trd lng="br">-<ind>perlez</ind></trd>
  <trd lng="ca">perlat</trd>
  <trd lng="cs">perliv&yacute;</trd>
  <trd lng="de">Perlmutt-</trd>
  <trd lng="en">pearly</trd>
  <trdgrp lng="fr">
     <trd>de <ind>perle</ind></trd>,
     <trd>perl&eacute;</trd>
   </trdgrp>
  <trdgrp lng="hu">
     <trd>gy&ouml;ngy-</trd>,
     <trd>gy&ouml;ngyszer&udblac;</trd>
   </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x771F;&#x73E0;&#x306E;  <pr>&#x3057;&#x3093;&#x3058;&#x3085;&#x306E;</pr></trd>,
    <trd>&#x771F;&#x73E0;&#x306E;&#x3088;&#x3046;&#x306A;  <pr>&#x3057;&#x3093;&#x3058;&#x3085;&#x306E;&#x3088;&#x3046;&#x306A;</pr></trd>,
    <trd>&#x771F;&#x73E0;&#x8272;&#x306E;  <pr>&#x3057;&#x3093;&#x3058;&#x3085;&#x3044;&#x308D;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">parelachtig</trd>
  <trd lng="pl">per&lstroke;owy</trd>
  <trd lng="ru">&c_zh;&c_je;&c_m;&c_ch;&c_u;&c_zh;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>perliv&yacute;</trd>,
    <trd>perlov&yacute;</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&c_je;&c_r;&#x43B;&#x43E;&c_v;&c_i;&c_j;</trd>
</drv>

<drv mrk="perl.0i">
   <kap><tld/>i</kap>
   <gra><vspec>ntr</vspec></gra>
   <snc mrk="perl.0i.KOMUNE">
     <dif>
       Eli&gcirc;i kun formo de <tld/>o:
       <ekz>
         la &scirc;vito <tld/>is sur lia frunto;
       </ekz>
       <ekz>
         el liaj okuloj <tld/>as malrapide larmoj.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_v;&c_y;&c_s;&c_t;&c_u;&c_p;&c_a;&c_c;&c_mol; <ind>&c_k;&c_r;&c_o;&c_p;&c_l;&c_ja;&c_m;&c_ib;</ind></trd>
  <trdgrp lng="br">
     <trd>perlezenni&ntilde;<klr tip="ind">
     (pizenna&ntilde;)</klr></trd>,
     <trd>pizenna&ntilde;</trd>
   </trdgrp>
  <trd lng="ca">perlejar</trd>
  <trd lng="cs">ronit slzy</trd>
  <trd lng="de">perlen</trd>
  <trd lng="fr">perler</trd>
  <trd lng="id"><baz>tetes</baz>menetes</trd>
  <trd lng="ja">&#x7389;&#x306B;&#x306A;&#x308B; 
  <pr>&#x305F;&#x307E;&#x306B;&#x306A;&#x308B;</pr></trd>
  <trd lng="nl">parelen</trd>
  <trd lng="pl">perli&cacute;</trd>
  <trd lng="sk">roni&tcaron; slzy</trd>
  <trd lng="uk">&c_v;&c_i;&c_s;&c_t;&c_u;&#x43F;&c_a;&c_t;&c_i; &#x43A;&c_r;&c_a;&#x43F;&#x43B;&#x44F;&#x43C;&c_i; (&#x43F;&c_r;&#x43E; &#x43F;&c_ib;&c_t; &c_ib;
  &c_t;.&#x43F;.)</trd>
</drv>
</art>
