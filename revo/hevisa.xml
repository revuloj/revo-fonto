<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>Hevisajd</rad>/o
</kap>
<drv mrk="hevisa.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Anglalingve:
      Oliver <trd lng="en">Heaviside</trd>, 1850-1925. Angla matematikisto
      kaj fizikisto.
    </dif>
  </snc>
  <trd lng="de">Heaviside</trd>
  <trd lng="fr">Heaviside</trd>
  <trd lng="nl">Oliver <ind>Heaviside</ind></trd>
  <trd lng="ru">&c_H;&c_je;&c_v;&c_i;&c_s;&c_a;&c_j;&c_d;</trd>
</drv>
<drv mrk="hevisa.h0a">
  <kap><tld lit="h"/>a</kap>
  <snc>
    <refgrp tip="vid">
      <ref cel="funkci.hevisajda0o"><tld lit="h"/>a funkcio</ref>.
    </refgrp>
  </snc>
    <trd lng="de">Heavisidesche</trd>
    <trd lng="en">Heaviside['s]</trd>
    <trd lng="fr">de <ind>Heaviside</ind></trd>
    <trd lng="nl">van <ind>Heaviside</ind></trd>
    <trd lng="ru">&c_H;&c_je;&c_v;&c_i;&c_s;&c_a;&c_j;&c_d;&c_a;</trd>
</drv>
</art>

