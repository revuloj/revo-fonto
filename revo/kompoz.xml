<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kompoz.xml,v 1.21 2023/12/03 09:58:54 revo Exp $">
<kap>
  <rad>kompozici</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kompoz.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="kompoz.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="dif" cel="kompon.0ajxo">Kompona&jcirc;o</ref>:
      <ekz>
        ludi improvizitan <tld/>on
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi frapados en la da&ubreve;ro de tuta horo diversajn klavojn,
        kaj vi ricevos tutan <tld/>on
        <fnt><bib>EE</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_p;&c_a;&c_z;&c_ib;&c_c;&c_y;&c_ja;</trd>
  <trd lng="ca">composici&oacute;</trd>
  <trdgrp lng="cs">
    <trd>kus</trd>,
    <trd>skladba</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Komposition</trd>,
    <trd>Tonst&uuml;ck</trd>
  </trdgrp>
  <trd lng="en">composition</trd>
  <trd lng="es">composici&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>composition<klr tip="ind"> (musicale)</klr></trd>,
    <trd>&oelig;uvre <ind>musicale</ind></trd>
  </trdgrp>
  <trd lng="ja">&#x4F5C;&#x54C1;  <pr>&#x3055;&#x304F;&#x3072;&#x3093;</pr></trd>
  <trd lng="nl">compositie</trd>
  <trd lng="pl">kompozycja</trd>
  <trd lng="pt">composi&ccedil;&atilde;o</trd>
  <trd lng="ro">compozi&#x21B;ie</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_o;&c_z;&c_i;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>kus</trd>,
    <trd>skladba</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&#x43E;&#x43C;&#x43F;&#x43E;&#x437;&#x438;&#x446;&#x456;&#x44F;</trd>
  <trd lng="zh">&#x6784;&#x56FE; <pr>g&#xF2;ut&#xFA;</pr></trd>
</drv>

<drv mrk="kompoz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="kompoz.0i.MUZ">
    <dif>
      <ref tip="dif" cel="kompon.0i.muzikajxon">Komponi<sncref/></ref>:
      <ekz>
        gepatroj <klr>...</klr> provis ordoni,
        ke ili sufoku la diajn povojn pentri, <tld/>i, kanti, verki 
        <fnt>
          <aut>C. Oxenford</aut>,
          <vrk><url ref="http://arkivo.esperanto-france.org/divers/oxenford/vagabond.htm">
          La Historio de la Esperantista Vagabonda Klubo</url></vrk>,
          <lok>1912</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="pl">komponowa&cacute;</trd>     
  <trd lng="ro">compune</trd>
</drv>
</art>
