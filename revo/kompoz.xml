<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kompozici</rad>/o <fnt>Z</fnt>
</kap>
<drv mrk="kompoz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <ref tip="dif" cel="kompon.0ajxo">kompona&jcirc;o</ref>
    <ekz>
      ludi improvizitan <tld/>on<fnt>Z</fnt>;
    </ekz>
    <ekz>
      vi frapados en la da&ubreve;ro de tuta horo diversajn klavojn,
      kaj vi ricevos tutan <tld/>on... &scirc;ajnas, ke nenio estas pli facila
      <fnt><bib>EE</bib></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_p;&c_a;&c_z;&c_ib;&c_c;&c_y;&c_ja;</trd>
  <trd lng="ca">composici&oacute;</trd>
  <trdgrp lng="de">
    <trd>Komposition</trd>,
    <trd>Tonst&uuml;ck</trd>
  </trdgrp>
  <trd lng="en">composition</trd>
  <trd lng="es">composici&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>composition<klr tip="ind"> (musicale)</klr></trd>,
    <trd>&oelig;uvre <ind>musicale</ind></trd>
  </trdgrp>
  <trd lng="nl">compositie</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_o;&c_z;&c_i;&c_c;&c_i;&c_ja;</trd>
</drv>
</art>
