<art mrk="$Id$">
<kap><rad>kompozici</rad>/o <fnt>Z</fnt></kap>
<drv mrk="kompoz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="vid" cel="kompon.0ajxo">Kompona&jcirc;o</ref>:
      <ekz>
        ludi improvizitan <tld/>on<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Komposition</trd>,
     <trd>Tonst&uuml;ck</trd>
  </trdgrp>
  <trd lng="en">composition</trd>
  <trd lng="nl">compositie</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_o;&c_z;&c_i;&c_c;&c_i;&c_ja;</trd>
</drv>
</art>
