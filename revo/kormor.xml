<art mrk="$Id$">
<kap>
<rad>kormoran</rad>/o
</kap>
<drv mrk="kormor.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="kormor1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Phalacrocorax</trd>), sin nutranta per fi&scirc;oj:
      <ekz>
        en &Ccirc;inujo oni dresas <tld/>ojn al fi&scirc;kaptado.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">kormor&aacute;n</trd>
  <trdgrp lng="de">
    <trd>Kormoran</trd>,
    <trd>Scharbe</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cormorant</trd>,
    <trd>shag</trd>
  </trdgrp>
  <trd lng="hu">k&aacute;r&oacute;katona</trd>
  <trd lng="nl">aalscholver</trd>
  <trd lng="pl">kormoran</trd>
  <trd lng="ru">&c_b;&c_a;&c_k;&c_l;&c_a;&c_n;</trd>
</drv>

</art>
