<art mrk="$Id: kormor.xml,v 1.20 2024/11/04 01:58:38 revo Exp $">
<kap>
  <rad>kormoran</rad>/o
</kap>
<drv mrk="kormor.0o">
  <kap><tld/>o</kap>
  <snc mrk="kormor.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="kormor1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Phalacrocorax</trd>), sin nutranta per fi&scirc;oj:
      <ekz>
        en &Ccirc;inujo oni dresas <tld/>ojn al fi&scirc;kaptado <fnt><bib>PV</bib></fnt>;
      </ekz>
      <ekz>
        mortis jam pro man&gcirc;o de venenitaj fi&scirc;oj <klr>[&#8230;]</klr> multaj mutaj cignoj kaj
        <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <vrk>Poluado: Cinike cianide &ndash; &ccirc;u respondecas Okcidento?</vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/3/36/Kormoran_auf_Norderney.jpg/640px-Kormoran_auf_Norderney.jpg"
      prm="CC BY 4.0"
      ><tld/>o (Phalacrocorax carbo), orientfrisa insulo Norderney
        <fnt>
          <aut>Stephan Sprinz, propra verko</aut>
          <lok><url ref="&WCW;/File:Kormoran_auf_Norderney.jpg"
              >https://commons.wikimedia.org/wiki/File:Kormoran_auf_Norderney.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_k;&c_l;&c_a;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>kormor&aacute;n</trd>,
    <trd>krkavec vodn&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kormoran</trd>,
    <trd>Scharbe</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cormorant</trd>,
    <trd>shag</trd>
  </trdgrp>
  <trd lng="es">cormor&aacute;n</trd>
  <trd lng="fr">cormoran</trd>
  <trd lng="hu">k&aacute;r&oacute;katona</trd>
  <trdgrp lng="ja">
    <trd>&#12454;</trd>,
    <trd>&#40284; <pr>&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">aalscholver</trd>
  <trd lng="pl">kormoran</trd>
  <trdgrp lng="pt">
    <trd>bigu&aacute;</trd>,
    <trd>cormor&atilde;o</trd>,
    <trd>corvo-marinho</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_a;&c_k;&c_l;&c_a;&c_n;</trd>
  <trd lng="sk">kormor&aacute;n</trd>
  <trdgrp lng="zh">
    <trd>&#40469; <pr>l&uacute;</pr></trd>,
    <trd>&#40469;&#40383; <pr>l&uacute;c&iacute;</pr></trd>
  </trdgrp>
</drv>
</art>
