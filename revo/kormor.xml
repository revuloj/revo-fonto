<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>kormoran</rad>/o
</kap>
<drv mrk="kormor.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="kormor.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Phalacrocorax</trd>), sin nutranta per fi&scirc;oj:
      <ekz>
        en &Hcirc;inujo oni dresas <tld/>ojn al fi&scirc;kaptado.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">kormor&aacute;n</trd>
  <trdgrp lng="de">
    <trd>Kormoran</trd>,
    <trd>Scharbe</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cormorant</trd>,
    <trd>shag</trd>
  </trdgrp>
  <trd lng="hu">k&aacute;r&oacute;katona</trd>
  <trd lng="pl">kormoran</trd>
  <trd lng="ru">&c_b;&c_a;&c_k;&c_l;&c_a;&c_n;</trd>
</drv>
<drv mrk="kormor.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="pelika.0oformaj">pelikanoformaj
      birdoj</ref> (<trd lng="la">Phalacrocoracidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Kormorane <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>
