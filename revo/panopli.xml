<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: panopli.xml,v 1.8 2024/02/22 06:26:38 revo Exp $">
<kap>
  <rad>panopli</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="panopli.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="panopli.0o.ARM">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">HER</uzo>
    <dif>
      <ref tip="vid" cel="komple2.0o">Kompleto</ref> de
      armiloj kaj &scirc;irmiloj de batalisto:
      <ekz>
        virinon la konspirantoj vestis per <tld/>o
        <klr>[&#8230;]</klr> kaj ili kondukis &scirc;in en la urbon
        <fnt>
          <aut>Herodoto, trad. S. Sarafian</aut>,
          <vrk>Historio</vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_y;&c_sh;&c_t;&c_u;&c_n;&c_a;&c_k; <klr>(&c_v;&c_a;&c_j;&c_s;&c_k;&c_o;&c_v;&c_y;)</klr></trd>
  <trd lng="cs">panoplie</trd>
  <trdgrp lng="de">
    <trd>Panoplie</trd>,
    <trd>R&uuml;stzeug</trd>
  </trdgrp>
  <trd lng="en">panoply</trd>
  <trd lng="fr">panoplie</trd>
  <trd lng="pl">ekwipunek</trd>
  <trd lng="sk">panoplia</trd>
  <trd lng="uk">&c_k;&c_o;&c_l;&c_je;&c_k;&c_c;&c_ib;&c_ja; &c_z;&c_b;&c_r;&c_o;&c_ji;</trd>
</drv>
</art>

