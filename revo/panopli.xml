<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: panopli.xml,v 1.7 2022/03/25 10:18:30 revo Exp $">
<kap>
  <rad>panopli</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="panopli.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="panopli.0o.ARM">
    <uzo tip="fak">MIL</uzo>
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="vid" cel="komple2.0o">Kompleto</ref> de
      armoj kaj &scirc;irmiloj de batalisto:
      <ekz>
        virinon la konspirantoj vestis per <tld/>o
        <klr>...</klr> kaj ili kondukis &scirc;in en la urbon
        <fnt>
          <aut>Herodoto, trad. S. Sarafian</aut>,
          <vrk>Historio</vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_y;&c_sh;&c_t;&c_u;&c_n;&c_a;&c_k; <klr>(&c_v;&c_a;&c_j;&c_s;&c_k;&c_o;&c_v;&c_y;)</klr></trd>
  <trd lng="cs">panoplie</trd>
  <trd lng="en">panoply</trd>
  <trd lng="fr">panoplie</trd>
  <trd lng="pl">ekwipunek</trd>
  <trd lng="sk">panoplia</trd>
  <trd lng="uk">&#x43A;&#x43E;&#x43B;&#x435;&#x43A;&#x446;&#x456;&#x44F; &#x437;&#x431;&#x440;&#x43E;&#x457;</trd>
</drv>
</art>

