<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: zakusk.xml,v 1.9 2024/07/23 21:28:04 revo Exp $">
<kap>
  <rad>zakusk</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="zakusk.0o">
  <kap><tld/>o</kap>
  <snc mrk="zakusk.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="mangx.antaux0ajxo.KUI">Anta&ubreve;man&gcirc;a&jcirc;o</ref>
      la&ubreve; oriente&ubreve;ropa maniero:
      <ekz>
        amo, vodko kaj <tld/>o
        <fnt>
          <aut>A. &Ccirc;e&hcirc;ov, trad. A. Kor&jcirc;enkov</aut>,
          <vrk><url ref="&LOdE;Libroj/V_bibl02.pdf">
          Sveda alumeto</url></vrk>,
          <lok>2004-2010</lok>
        </fnt>!
      </ekz>
      <ekz>
        <klr>[la]</klr> edzo de bofilino de la caro <klr>[proponis]</klr> al &leftquot;la freneza
        mona&hcirc;o&rightquot; <nom>Rasputin</nom> en sia palaco venenigitan vinon kaj <tld/>ojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=65478">Grigorij Rasputin</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bufed.0o.servo">bufedo<sncref/></ref>,
      <ref cel="frand.0ajxo">franda&jcirc;o</ref>,
      <ref cel="luncx.0o">lun&ccirc;o</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_k;&c_u;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_je;&c_d;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="ar">&a_z;&a_A;&a_k;&a_w;&a_s;&a_k;&a_y;</trd>
  <trd lng="cs">z&aacute;kusek <klr>(k vodce| k v&iacute;nu)</klr></trd>
  <trdgrp lng="de">
    <trd>H&auml;ppchen</trd>,
    <trd>Sakuska</trd>,
    <trd>Snack</trd>
  </trdgrp>
  <trd lng="en">Zakuski</trd>
  <trd lng="el">&zeta;&alfa;&kappa;&omikron;&ypsilon_ton;&sigma;&kappa;&jota;</trd>
  <trd lng="fr">zakouski<klr> (hors-d'&oelig;uvre russe)</klr></trd>
  <trd lng="ja">&#x30B6;&#x30AF;&#x30FC;&#x30B9;&#x30AD;</trd>
  <trdgrp lng="pl">
    <trd>zak&aogonek;ska</trd>,
    <trd>przystawka</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>z&aacute;kusok <klr>(k vodke, k v&iacute;nu)</klr></trd>,
    <trd>z&aacute;kusok <klr>(k vodke| k v&iacute;nu)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_k;&c_u;&c_s;&c_k;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x4E00;&#x5C0F;&#29255; <pr>y&#xEC;xi&#x1CE;opi&#xE0;n</pr></trd>,
    <trd>&#x7D30;&#x5C51; <pr>x&#xEC;xi&#xE8;</pr></trd>,
    <trd>&#x7EC6;&#x5C51; <pr>x&#xEC;xi&#xE8;</pr></trd>,
    <trd>&#x70B9;&#x5FC3; <pr>di&#x1CE;nx&#x12B;n</pr></trd>,
    <trd>&#x9EDE;&#x5FC3; <pr>di&#x1CE;nx&#x12B;n</pr></trd>
  </trdgrp>
</drv>
</art>

