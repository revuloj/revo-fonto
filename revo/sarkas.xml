<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>sarkasm</rad>/o
</kap>
<drv mrk="sarkas.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Akra kaj mordanta moko:
      <ekz>
        <tld/>a rideto<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_r;&c_k;&c_a;&c_z;&c_m;</trd>
  <trd lng="de">Sarkasmus</trd>
  <trd lng="en">sarcasm</trd>
  <trd lng="es">sarcasmo</trd>
  <trd lng="fr">sarcasme</trd>
  <trdgrp lng="hu">
    <trd>szarkazmus</trd>,
    <trd>mar&oacute; <ind>g&uacute;ny</ind></trd>
  </trdgrp>
  <trd lng="nl">sarcasme</trd>
  <trd lng="ru">&c_s;&c_a;&c_r;&c_k;&c_a;&c_z;&c_m;</trd>
  <trd lng="sk">sarkazmus</trd>
</drv>

<drv mrk="sarkas.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Akre, vunde moka.
    </dif>
    <ref tip="sin" cel="sardon.0a">sardona</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_r;&c_k;&c_a;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_a;&c_r;&c_d;&c_a;&c_n;&c_ib;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>sarcastic</trd>,
    <trd>sardonic</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>sarcastique</trd>,
    <trd>sardonique</trd>
  </trdgrp>
</drv>
</art>
