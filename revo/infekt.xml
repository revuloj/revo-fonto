<drv mrk="infekt.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="infekt.0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="komuni1.0i.transporti">Komuniki</ref> 
      al iu &gcirc;ermojn de malsano:
      <ekz>
        tiu &ccirc;i malsano <tld/>is jam multajn urbojn
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu sola malsanulo <tld/>is en kanada urbo Toronto centojn da homoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2003/008129.html">Propagando dan&gcirc;era</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili rezulte de senordaj amoraj kontaktoj riskas <tld/>i&gcirc;i je aidoso kaj
        reveninte al siaj familioj povas <tld/>i edzinojn
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2004/008160.html">Afriko kaj aidoso</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        la viruson kunportas la migrantaj birdoj, kaj ili <tld/>as bredejajn kortobirdojn
        <fnt>
          <bib>Monato</bib>
          <aut>Thierry Tailhades</aut>
          <vrk><url ref="&Monato;/2006/008985.php">Gurdado de onidiroj</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i homojn per prionoj rezulte de uzado de sango
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2006/008861.php">Nova etapo en biologio kaj medicino</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu fava &scirc;afo tutan &scirc;afaron <tld/>as
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kontagx.0i">konta&gcirc;i</ref>
    </refgrp>
    <trdgrp lng="ms">
     <trd>menjangkiti<klr tip="amb"> (perubatan)</klr></trd>, 
      </trdgrp>
    <trdgrp lng="pt">
     <trd>infeccionar<klr tip="amb"> (medicina)</klr></trd>, 
     <trd>contaminar<klr tip="amb"> (medicina)</klr></trd>,
     <trd>contagiar<klr tip="amb"> (medicina)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_r;&c_a;&c_z;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_i;&c_n;&c_f;&c_i;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="th">&#xE17;&#xE33;&#xE43;&#xE2B;&#xE49;&#xE15;&#xE34;&#xE14;&#xE40;&#xE0A;&#xE37;&#xE49;&#xE2D;</trd>
  </snc>
  <snc mrk="infekt.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Komuniki malvirton a&ubreve; malbona&jcirc;on:
      <ekz>
        la senkulpan animon de la infano ili <tld/>as per la plej groteskaj mensogoj
        <fnt>
          <bib>VaK</bib>
          <lok>Dekunua &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        per la freneza&jcirc;oj de la plej diversaj erardoktrinoj mi <tld/>is la puran
        publikan vivon
        <fnt>
          <bib>VaK</bib>
          <lok>Deksepa &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&gcirc;i]</klr> <tld/>is vin per la manio fari&gcirc;i granda
        homo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        korporaciaj skandaloj <tld/>as &ccirc;iun nivelon de la registaro
        <fnt>
          <bib>Monato</bib>
          <aut>Elson B. SNOW</aut>
          <vrk><url ref="&Monato;/2003/008006.html">Dio, benu Usonon! (2)</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ju&gcirc;is la grandan mal&ccirc;astistinon, kiu <tld/>is
        la teron per sia mal&ccirc;asteco
        <fnt><bib>NT</bib>, <lok>&Apo; 19:2</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="korupt.0i.morale">korupti<sncref/></ref>,
      <ref cel="sku.0i.FIG">skui<sncref/></ref>,
      <ref cel="venen.0i">veneni</ref>
    </refgrp>
    <trd lng="pt">contaminar<klr tip="amb"> (figurado)</klr></trd>
  </snc>
  <snc mrk="infekt.0i.rido">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Komuniki kaj pli<ref tip="super" cel="vast.0igi">vastigi</ref> 
      iun ajn kutimon, senton a&ubreve; pensmanieron:
      <ekz>
        &scirc;ia labor&gcirc;ojo <tld/>is anka&ubreve; la servantojn
        <fnt>
          <bib>HsT</bib>
          <lok>Heredanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la saluta frenezo balda&ubreve; <tld/>is anka&ubreve; la &scirc;ipon de l'
        kronprinco: la oficiroj, soldatoj kaj remistoj,
        kunpremitaj en unu amason, vete kriis
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 17a.</lok></fnt>;
      </ekz>
      <ekz>
        la vigla etoso de la kajuto <tld/>is min kaj igis min entreprenema
        <fnt>
          <bib>Kiso</bib>
          <lok>La Sakramento De La Komunio</lok>
        </fnt>;
      </ekz><ekz>
        ne mirinde, ke lia stato teruris &dash; ke &gcirc;i <tld/>is min
        <fnt>
          <aut>E.A. Poe, tr. Edmund Grimley Evans</aut>:
          <vrk>La falo de la domo Usher</vrk>.
          <lok>Sezonoj, 2002</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="inokul.0i">inokuli</ref>,
      <ref cel="inspir.0i">inspiri</ref>
    </refgrp>
    <trd lng="pt">contagiar<klr tip="amb"> (figurado)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>infikovat</trd>,
    <trd>nakazit n&ecaron;&ccaron;&iacute;m</trd>,
    <trd>zan&eacute;st choroboplodn&eacute; z&aacute;rodky</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>anstecken</trd>,
    <trd>infizieren</trd>
  </trdgrp>
  <trd lng="en">infect</trd>
  <trd lng="es">infectar</trd>
  <trdgrp lng="fr">
    <trd>infecter</trd>,
    <trd>contaminer<klr tip="ind"> (infecter)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fert&odblac;z</trd>,
    <trd>megfert&odblac;z</trd>
  </trdgrp>
  <trd lng="id">meng<ind>infeksi</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x611F;&#x67D3;&#x3055;&#x305B;&#x308B;  <pr>&#x304B;&#x3093;&#x305B;&#x3093;&#x3055;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x75C5;&#x6C17;&#x3092;&#x3046;&#x3064;&#x3059;  <pr>&#x3073;&#x3087;&#x3046;&#x304D;&#x3092;&#x3046;&#x3064;&#x3059;</pr></trd>,
    <trd>&#x611F;&#x5316;&#x3059;&#x308B;  <pr>&#x304B;&#x3093;&#x304B;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>aansteken</trd>,
    <trd>infecteren</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_r;&c_a;&c_z;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>infikova&tcaron;</trd>,
    <trd>nakazi&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_ib;&c_n;&c_f;&c_ib;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x611F;&#x67D3; <pr>g&#x1CE;nr&#x1CE;n</pr></trd>,
    <trd>&#x4F20;&#x67D3; <pr>chu&#xE1;nr&#x1CE;n</pr></trd>,
    <trd>&#x4F20; <pr>chu&#xE1;n</pr></trd>,
    <trd>&#x67D3; <pr>r&#x1CE;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="infekt.0o0ado">
  <kap><tld/>o, <var><kap><tld/>ado</kap></var></kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Invado de mikroboj en organismon; rezultanta stato:
      <ekz>
        <tld/>o de lepro
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 24:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi havas &ccirc;iujn simptomojn de <tld/>o
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XXVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la progreso de l&#x2019; malsano fari&gcirc;is rapida, kiel ordinare okazas post la &hcirc;irurgiaj
        <tld/>oj
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XXVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o, &ccirc;u bakteria, &ccirc;u virusa
        <fnt>
          <bib>ChR</bib>
          <lok>Ne nur tamtamado</lok>
        </fnt>;
      </ekz>
      <ekz>
        hantovirusa <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2004/008278.html">Damokla glavo super la homaro</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ado per patologiaj prionoj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2006/008861.php">Nova etapo en biologio kaj medicino</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &gcirc;ustan diagnozon de HIV-<tld/>o oni povas meti nur post laboratoriaj esploroj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2004/008160.html">Afriko kaj aidoso</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">KOMP</uzo>
        tiu viruso sendas kopiojn de si mem al &ccirc;iuj retadresoj, kiujn &gcirc;i povas trovi en
        la <tld/>ita komputilo
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Peeraerts</aut>
          <vrk><url ref="&Monato;/2003/008183.html">Sobig-F</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>
  </snc>
  <trd lng="bo">&t_-a;&t_ga;&t_naro;&t_sa;&t_tsheg;&t_na;&t_da;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>infekce</trd>,
    <trd>infektov&aacute;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ansteckung</trd>,
    <trd>Infektion</trd>
  </trdgrp>
  <trd lng="es">infecci&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>infection</trd>,
    <trd>contagion</trd>
  </trdgrp>
  <trd lng="hu">fert&odblac;z&eacute;s</trd>
  <trd lng="id">infeksi</trd>
  <trdgrp lng="ja">
    <trd>&#x611F;&#x67D3;  <pr>&#x304B;&#x3093;&#x305B;&#x3093;</pr></trd>,
    <trd>&#x611F;&#x5316;  <pr>&#x304B;&#x3093;&#x304B;</pr></trd>
  </trdgrp>
  <trd lng="pt">infec&ccedil;&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_f;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_r;&c_a;&c_z;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">infekcia</trd>
  <trd lng="th">&#xE01;&#xE32;&#xE23;&#xE15;&#xE34;&#xE14;&#xE40;&#xE0A;&#xE37;&#xE49;&#xE2D;</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_r;&c_a;&c_z;&c_a;</trd>,
    <trd>&c_ib;&c_n;&c_f;&c_je;&c_k;&c_c;&c_ib;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x611F;&#x67D3; <pr>g&#x1CE;nr&#x1CE;n</pr></trd>,
    <trd>&#x4FB5;&#x67D3; <pr>q&#x12B;nr&#x1CE;n</pr></trd>,
    <trd>&#x53D1;&#x708E; <pr>f&#x101;y&#xE1;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="infekt.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="infekt.0a.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Rilata al <tld/>o; ka&ubreve;zita de <tld/>o; ka&ubreve;zanta <tld/>on:
      <ekz>
        post <ind><tld/>a malsano</ind> oni ofte bruligas
        la vestojn de la malsanulo
        <fnt><bib>F</bib>, <lok>&FE;39</lok></fnt>;
        <trdgrp lng="ru">
          <trd>&c_z;&c_a;&c_r;&c_a;&c_z;&c_n;&c_a;&c_ja; &c_b;&c_o;&c_l;&c_je;&c_z;&c_n;&c_mol;</trd>,
          <trd>&c_i;&c_n;&c_f;&c_je;&c_k;&c_c;&c_i;&c_o;&c_n;&c_n;&c_o;&c_je; &c_z;&c_a;&c_b;&c_o;&c_l;&c_je;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        la juna kuracisto pali&gcirc;is, rekonante la timindajn simptomojn de l&#x2019; <tld/>a
        peritoneito
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a epidemio
        <fnt>
          <bib>VaK</bib>
          <lok>Dekunua &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu <tld/>a infuza&jcirc;o, en kiu senbride orgias
        baciloj kun mikroboj, kaj kiun en <nom>Marseille</nom>
        vi nomas akvo
        <fnt>
          <aut>Raymond Schwartz</aut>:
          <vrk>La puto de Paupel&egrave;che</vrk>.
          <lok>La Nica literatura revuo, 2:6</lok>
        </fnt>!
      </ekz>
      <ekz>
        <tld/>aj proteinaj partikloj estas kapablaj reprodukti&gcirc;i sen partoprenado de DNA
        a&ubreve; RNA
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2006/008861.php">Nova etapo en biologio kaj medicino</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_z;&c_a;&c_r;&c_a;&c_z;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="infekt.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kapabla <tld/>i<sncref ref="infekt.0i.rido"/>:
      <ekz>
        la interkomuniki&gcirc;o estas <tld/>a, &scirc;i diris, nun la kelneroj e&ccirc; volas esti
        kavaliroj
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a ridado.
      </ekz>
    </dif>
    <trd lng="ru">&c_z;&c_a;&c_r;&c_a;&c_z;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trd lng="bo">&t_-a;&t_ga;&t_naro;&t_sa;&t_tsheg;&t_na;&t_da;&t_tsheg;&t_ca;&t_na;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>infek&ccaron;n&iacute;</trd>,
    <trd>naka&zcaron;liv&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ansteckend</trd>,
    <trd>infekti&ouml;s</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>infectieux</trd>,
    <trd>contagieux</trd>
  </trdgrp>
  <trd lng="hu">fert&odblac;z&odblac;</trd>
  <trdgrp lng="ja">
    <trd>&#x611F;&#x67D3;&#x306E;  <pr>&#x304B;&#x3093;&#x305B;&#x3093;&#x306E;</pr></trd>,
    <trd>&#x611F;&#x67D3;&#x6027;&#x306E;  <pr>&#x304B;&#x3093;&#x305B;&#x3093;&#x305B;&#x3044;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">besmettelijk</trd>
  <trdgrp lng="pt">
    <trd>infecto</trd>, 
    <trd>contagioso</trd> 
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chyt&lcaron;av&yacute;</trd>,
    <trd>infek&ccaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ib;&c_n;&c_f;&c_je;&c_k;&c_c;&c_ib;&c_j;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_r;&c_a;&c_z;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_r;&c_a;&c_z;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4F20;&#x67D3; <pr>chu&#xE1;nr&#x1CE;n</pr></trd>,
    <trd>&#x611F;&#x67D3;&#x6027; <pr>g&#x1CE;nr&#x1CE;nx&#xEC;ng</pr></trd>,
    <trd>&#x6613;&#x4F20;&#x67D3; <pr>y&#xEC;zhu&#xE0;nr&#x1CE;n</pr></trd>,
    <trd>&#x4F1A;&#x611F;&#x67D3; <pr>hu&#xEC;g&#x1CE;nr&#x1CE;n</pr></trd>,
    <trd>&#x5F71;&#x54CD;&#x522B;&#x4EBA; <pr>y&#x1D0;ngxi&#x1CE;ngbi&#xE9;r&#xE9;n</pr></trd>,
    <trd>&#x62DB;&#x4EBA; <pr>zh&#x101;or&#xE9;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="infekt.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">MED</uzo>
  <snc mrk="infekt.0ajxo.antajxo">
    <dif>
      <tld lit="I"/>anta&jcirc;o, substanco entenanta mikrobojn, kiu
      ka&ubreve;zas <tld/>an malsanon:
      <ekz>
        <klr>[se]</klr> la <tld/>a&jcirc;o estos verdeta a&ubreve; ru&gcirc;eta <klr>[&#x2026;]</klr>, 
        tiam &gcirc;i estas <tld/>a&jcirc;o de lepro
        <fnt><bib>MT</bib><lok>&Lev; 13:47&ndash;49.</lok></fnt>;
      </ekz>
      <ekz>
        liajn doktrinojn <klr>[ili]</klr> nomis <tld/>a&jcirc;o kiu renversas la civilizacion
        <fnt>
          <bib>VaK</bib>
          <lok>Deka &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la akuzito intencis devojigi la animojn de la junuloj <klr>[&#x2026;]</klr> kaj pu&scirc;i
        ilin al malhonora <tld/>a&jcirc;o
        <fnt>
          <bib>SatirRak</bib>
          <lok>Pythagoras</lok>
        </fnt>.
      </ekz>
    </dif>
