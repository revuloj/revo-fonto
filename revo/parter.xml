<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: parter.xml,v 1.26 2024/02/26 09:22:28 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>parter</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="parter.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="parter.0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Parto de <ref tip="malprt" cel="teatr.0ejo">teatreja</ref>
      &ccirc;ambrego &ccirc;e tereta&gcirc;o inter la orkestrejo kaj la
      funda muro:
      <ekz>
        mi ekvidis en la mezo de la koncertejo sur <tld/>aj sidlokoj tri mariste
        vestitajn virojn
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &hcirc;orejo, &scirc;tuparoj, <tld/>o, amfiteatro, muroj, &ccirc;io <klr>[&#8230;]</klr> ektondris en
        potenca kriego
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        dum koncerto a&ubreve; prelego mi kompreneble rajtas promeni tra la <tld/>o
        <klr>[&#8230;]</klr>, mia rolo ja seriozas: tiuj fotoj fari&gcirc;os altrangaj dokumentoj
        <fnt>
          <bib>Kon11</bib>
          <lok>Blogo de fotemulo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="balkon.0o.TEA">balkono</ref>,
      <ref cel="galeri.0o.TEA">galerio<sncref/></ref>
    </refgrp>
     <trd lng="be">&c_p;&c_a;&c_r;&c_t;&c_e;&c_r;</trd>
     <trd lng="br">leur<klr tip="ind"> (c'hoariva)</klr></trd>
     <trd lng="ca">fossa de l'orquestra</trd>
     <trd lng="de">Parkett</trd>
     <trd lng="es">platea</trd>
     <trd lng="fr">parterre</trd>
     <trd lng="nl">parterre</trd>
     <trd lng="pl">parter</trd>
     <trd lng="pt">plat&eacute;ia <klr>(parte de teatro)</klr></trd>
     <trd lng="ro">parter</trd>
     <trd lng="ru">&c_p;&c_a;&c_r;&c_t;&c_je;&c_r;</trd>
  </snc>
  <snc mrk="parter.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ke><ref tip="dif" cel="etagx.ter0o.ARKI">Tereta&gcirc;o</ref></ke>:
      <ekz>
         la ru&gcirc;aj kurtenoj en la &ccirc;ambroj de la <tld/>o kaj la
         ne&gcirc;e blankaj flankkurtenoj en la malgranda dormo&ccirc;ambro supre
          <fnt>
            <bib>BdV</bib>
          </fnt>;
      </ekz>
      <ekz>
         en la <tld/>o estis nur magazeno
          <fnt>
            <bib>Fab2</bib>
            <lok>La ombro</lok>
          </fnt>;
      </ekz>
      <ekz>
         de <tld/>o al balkono zumadis Esperanto
         <fnt>
           <bib>VivZam</bib>
         </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_je;&c_r;&c_sh;&c_y;
    <ind>&c_p;&c_a;&c_v;&c_je;&c_r;&c_h;</ind></trd>
    <trd lng="br">rez-an-douar</trd>
    <trd lng="ca">planta baixa</trd>
    <trd lng="de">Erdgeschoss</trd>
    <trd lng="en">ground floor</trd>
    <trd lng="fr">rez-de-chauss&eacute;e</trd>
    <trd lng="pl">klomb</trd>
    <trd lng="ro">clomb&abreve;</trd>
    <trd lng="ru">&c_n;&c_i;&c_zh;&c_n;&c_i;&c_j;
    &c_e;&c_t;&c_a;&c_zh;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>kv&ecaron;tinov&aacute; zahrada</trd>,
    <trd>parter</trd>,
    <trd>p&rcaron;&iacute;zem&iacute; hledi&scaron;t&ecaron;</trd>
  </trdgrp>
  <trd lng="de">Parterre</trd>
  <trd lng="hu">f&ouml;ldszint<klr tip="ind"> (sz&iacute;nh&aacute;zban
  is)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#19968;&#38542;&#24109;  <pr>&#12402;&#12392;&#12363;&#12356;&#12379;&#12365;</pr></trd>,
    <trd>&#24179;&#22303;&#38291;  <pr>&#12402;&#12425;&#12393;&#12414;</pr></trd>,
    <trd/>,
    <trd>&#22823;&#33457;&#22727;  <pr>&#12384;&#12356;&#12363;&#12384;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">benedenverdieping</trd>
  <trdgrp lng="pt">
     <trd>t&eacute;rreo</trd>,
     <trd>r&eacute;s-do-ch&atilde;o</trd>
   </trdgrp>
  <trd lng="sk">pr&iacute;zemie</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_a;&c_r;&c_t;&c_je;&c_r; (&c_u; &c_t;&c_je;&c_a;&c_t;&c_r;&c_ib;)</trd>,
    <trd>&c_n;&c_i;&c_zh;&c_n;&c_ib;&c_j; &c_p;&c_o;&c_v;&c_je;&c_r;&c_h;</trd>,
    <trd>&c_k;&c_v;&c_ib;&c_t;&c_k;&c_o;&c_v;&c_a; &c_k;&c_l;&c_u;&c_m;&c_b;&c_a;</trd>
  </trdgrp>
</drv>
</art>
