<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: parter.xml,v 1.25 2023/10/20 14:25:20 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>parter</rad>/o <fnt><bib>Fab2</bib></fnt>
</kap>
<drv mrk="parter.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="parter.0o.TEA">
     <uzo tip="fak">TEA</uzo>
     <dif>
       Parto de <ref tip="malprt" cel="teatr.0ejo">teatreja</ref>
       &ccirc;ambrego &ccirc;e tereta&gcirc;o inter la orkestrejo kaj la
       funda muro.
     </dif>                      
     <trd lng="be">&c_p;&c_a;&c_r;&c_t;&c_e;&c_r;</trd>
     <trd lng="br">leur<klr tip="ind"> (c'hoariva)</klr></trd>
     <trd lng="ca">fossa de l'orquestra</trd>
     <trd lng="de">Parkett</trd>
     <trd lng="es">platea</trd>
     <trd lng="fr">parterre</trd>
     <trd lng="nl">parterre</trd>
     <trd lng="pl">parter</trd>
     <trd lng="pt">plat&eacute;ia <klr>(parte de teatro)</klr></trd>
     <trd lng="ro">parter</trd>
     <trd lng="ru">&c_p;&c_a;&c_r;&c_t;&c_je;&c_r;</trd>
  </snc>
  <snc mrk="parter.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="dif" cel="etagx.ter0o.ARKI">Tereta&gcirc;o</ref>:
      <ekz>
         la ru&gcirc;aj kurtenoj en la &ccirc;ambroj de la <tld/>o kaj la
         ne&gcirc;e blankaj flankkurtenoj en la malgranda dormo&ccirc;ambro supre
          <fnt>
            <bib>BdV</bib>
          </fnt>;
      </ekz>
      <ekz>
         en la <tld/>o estis nur magazeno
          <fnt>
            <bib>Fab2</bib>
            <lok>La ombro</lok>
          </fnt>;
      </ekz>
      <ekz>
         de <tld/>o al balkono zumadis Esperanto
         <fnt>
           <bib>VivZam</bib>
         </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_je;&c_r;&c_sh;&c_y;
    <ind>&c_p;&c_a;&c_v;&c_je;&c_r;&c_h;</ind></trd>  
    <trd lng="br">rez-an-douar</trd>
    <trd lng="ca">planta baixa</trd>
    <trd lng="de">Erdgeschoss</trd>
    <trd lng="en">ground floor</trd>
    <trd lng="fr">rez-de-chauss&eacute;e</trd>
    <trd lng="pl">klomb</trd>
    <trd lng="ro">clomb&abreve;</trd>
    <trd lng="ru">&c_n;&c_i;&c_zh;&c_n;&c_i;&c_j;
    &c_e;&c_t;&c_a;&c_zh;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>kv&ecaron;tinov&aacute; zahrada</trd>,
    <trd>parter</trd>,
    <trd>p&rcaron;&iacute;zem&iacute; hledi&scaron;t&ecaron;</trd>
  </trdgrp>
  <trd lng="de">Parterre</trd>
  <trd lng="hu">f&ouml;ldszint<klr tip="ind"> (sz&iacute;nh&aacute;zban
  is)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x4E00;&#x968E;&#x5E2D;  <pr>&#x3072;&#x3068;&#x304B;&#x3044;&#x305B;&#x304D;</pr></trd>,
    <trd>&#x5E73;&#x571F;&#x9593;  <pr>&#x3072;&#x3089;&#x3069;&#x307E;</pr></trd>,
    <trd/>,
    <trd>&#x5927;&#x82B1;&#x58C7;  <pr>&#x3060;&#x3044;&#x304B;&#x3060;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">benedenverdieping</trd>
  <trdgrp lng="pt">
     <trd>t&eacute;rreo</trd>,
     <trd>r&eacute;s-do-ch&atilde;o</trd>
   </trdgrp>
  <trd lng="sk">pr&iacute;zemie</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x430;&#x440;&#x442;&#x435;&#x440; (&#x443; &#x442;&#x435;&#x430;&#x442;&#x440;&#x456;)</trd>,
    <trd>&#x43D;&#x438;&#x436;&#x43D;&#x456;&#x439; &#x43F;&#x43E;&#x432;&#x435;&#x440;&#x445;</trd>,
    <trd>&#x43A;&#x432;&#x456;&#x442;&#x43A;&#x43E;&#x432;&#x430; &#x43A;&#x43B;&#x443;&#x43C;&#x431;&#x430;</trd>
  </trdgrp>
</drv>
</art>
