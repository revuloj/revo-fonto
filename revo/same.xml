<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>same</rad>/o
</kap>

<drv mrk="same.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:popolanoj" cel="an.0o">Ano</ref>
      de la &ccirc;efa gento
      de <tld lit="S"/>ujo:
      <ekz>
        la prafinnoj kaj <tld/>oj ege timis la urson
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="http://www.monato.net/2009/009756.php">Praa religia temo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj genetike rimarkinde diferencas de siaj finnaj najbaroj, sed parolas proksiman lingvon
        <fnt>
          <bib>Monato</bib>
          <aut>H&egrave;ctor Al&ograve;s i Font</aut>
          <vrk><url ref="http://www.monato.net/2013/011015.php">La turka: pli granda, pli diversa familio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        videblas anka&ubreve; pratempaj sanktejoj de <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="http://www.monato.net/2015/011433.php">Krozi la&ubreve; la norvega marbordo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="lapon.0o">lapono</ref>
  </snc>
  <trd lng="de">Lappl&auml;nder</trd>
  <trdgrp lng="en">
    <trd>Laplander</trd>,
    <trd>Sami</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>Lap</trd>,
    <trd>Laplander</trd>
  </trdgrp>
</drv>

<drv mrk="same.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj a&ubreve; <tld lit="S"/>ujo:
      <ekz>
        ekde a&ubreve;gusto 2006 eblas uzi anka&ubreve; literojn de tri <tld/>aj lingvoj parolataj en Finnlando
        <fnt>
          <bib>Monato</bib>
          <aut>Jukka Pietil&auml;inen</aut>
          <vrk><url ref="http://www.monato.net/2006/009040.php">Interrete anka&ubreve; per naciaj literoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la &leftquot;skandinava kruco&#8221; <klr>[&#8230;]</klr> estas uzata en &ccirc;iuj oficialaj flagoj de Nordio, escepte de la <tld/>a
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="http://www.monato.net/2014/011096.php">Skandinava veksilologio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        tiu &ccirc;i formo havas rimarkindan disvasti&gcirc;on, ekzemple en la
        kaza&hcirc;a (<nac>Alma Ata</nac>, patro de pomoj), turka (<nac>Atat&uuml;rk</nac>, patro 
        de la turkoj), nord<tld/>a (<nac>&aacute;h&ccaron;&ccaron;i</nac>, patro)
        <fnt>
          <bib>Monato</bib>
          <aut>Frieder Weigold</aut>
          <vrk><url ref="https://www.monato.be/2004/008420.html">Hinde&ubreve;ropaj etimoj (3)</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="lapon.0a">lapona</ref>
  </snc>
  <trd lng="de">lappl&auml;ndisch</trd>
  <trd lng="en">Laplandic</trd>
  <trd lng="fy">Lapl&acirc;nsk</trd>
  <trd lng="nl">Laplands</trd>
</drv>

<drv mrk="same.S0ujo">
  <kap><tld lit="S"/>ujo</kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:regionoj" cel="region.0o.land">Regiono</ref>
      kovranta partojn de <ref tip="vid"
      cel="finn.F0lando">Finnlando</ref>, <ref tip="vid"
      cel="norveg.N0ujo">Norvegujo</ref>, <ref tip="vid"
      cel="sved.S0ujo">Svedujo</ref> kaj <ref tip="vid"
      cel="rus.R0ujo">Rusujo</ref> (24&#730;E, 68&#730;N):
      <ekz>
        en <tld lit="S"/>ujo kaj Rusio homoj pa&scirc;tas la boacojn, sed en Kanado la boacoj estas sova&gcirc;aj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=6790">Boaco</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="lapon.L0ujo">Laponujo</ref>
  </snc>
  <trd lng="ca">Lap&ograve;nia</trd>
  <trd lng="de">Lappland</trd>
  <trd lng="en">Lapland</trd>
  <trd lng="fy">Lapl&acirc;n</trd>
  <trd lng="it">Lapponia</trd>
  <trd lng="nl">Lapland</trd>
  <trd lng="ru">&c_L;&c_a;&c_p;&c_l;&c_a;&c_n;&c_d;&c_i;&c_ja;</trd>
</drv>
</art>

