<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: temper2.xml,v 1.1 2018/06/21 11:10:13 revo Exp $">
<kap>
  <rad>temper</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="temper2.0o">
  <kap><tld/>o</kap>
  <snc mrk="temper2.0o.KOL">
    <dif>
      Pentra <ref tip="super" cel="farb.0o.KOL">farbo</ref> farita per
      oleeca emulsio en akvo, en kiu la olea parto konsistas en
      ovo-flavo, kazeino a&ubreve; alia simila substanco:
      <ekz>
        likva <tld/>o povas putri se oni ne uzas &gcirc;in sufi&ccirc;e rapide
        <fnt>
          <aut>F. van Hertrooij</aut>
          <vrk><url
          ref="https://scivolemo.wordpress.com/2018/02/14/la-scienco-de-arto/">
          La scienco de arto</url></vrk>
          <lok>Scivolemo, 2018-02-14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="temper2.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Pentra&jcirc;o farita per <tld/>o-farbo.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>d&eacute;trempe</trd>,
    <trd>tempera</trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
-->
</vortaro>