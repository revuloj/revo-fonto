<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: temper2.xml,v 1.6 2024/01/16 21:22:47 revo Exp $">
<kap>
  <rad>temper</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="temper2.0o">
  <kap><tld/>o</kap>
  <snc mrk="temper2.0o.KOL">
    <dif>
      Pentra <ref tip="super" cel="farb.0o.KOL">farbo</ref> farita per
      oleeca emulsio en akvo, en kiu la olea parto konsistas en
      ovo-flavo, kazeino a&ubreve; alia simila substanco:
      <ekz>
        likva <tld/>o povas putri se oni ne uzas &gcirc;in sufi&ccirc;e rapide
        <fnt>
          <aut>F. van Hertrooij</aut>
          <vrk><url ref="https://scivolemo.wordpress.com/2018/02/14/la-scienco-de-arto/">
          La scienco de arto</url></vrk>
          <lok>Scivolemo, 2018-02-14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="temper2.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Pentra&jcirc;o farita per <tld/>o-farbo.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_m;&c_p;&c_e;&c_r;&c_a;</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;trempe</trd>,
    <trd>tempera</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4E00;&#x77AC;  <pr>&#12356;&#12387;&#12375;&#12421;&#12435;</pr></trd>,
    <trd>&#x77AC;&#26178;  <pr>&#12375;&#12421;&#12435;&#12376;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>tempera</trd>,
    <trd>malarstwo  <ind>temperowe</ind></trd>
  </trdgrp>
  <trd lng="uk">&c_t;&c_je;&#x43C;&#x43F;&c_je;&c_r;&c_a; (&c_f;&c_a;&c_r;&c_b;&c_a;)</trd>
</drv>
</art>

