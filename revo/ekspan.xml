<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ekspan.xml,v 1.25 2023/11/07 19:22:36 revo Exp $">
<kap>
  <rad>ekspansi</rad>/o <fnt>Z</fnt> <fnt><bib>ZR</bib></fnt>
</kap>
<drv mrk="ekspan.0o">
  <kap><tld/>o</kap>
  <snc mrk="ekspan.0o.KOMUNE">
    <dif>
      Plivasti&gcirc;o, plietendi&gcirc;o.
    </dif>
    <subsnc mrk="ekspan.0o.AST">
      <uzo tip="fak">AST</uzo>
      <dif>
        Vasti&gcirc;o en la kosmo:
        <ekz>
          la <tld/>o de la universo.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="ekspan.0o.POL">
      <uzo tip="fak">JUR</uzo>
      <uzo tip="fak">EKON</uzo>
      <dif>
        Vasti&gcirc;o geografia:
        <ekz>
          kolonia <tld/>o de &scirc;tato.
        </ekz>
      </dif>
    </subsnc>
    <trdgrp lng="be">
      <trd>&c_e;&c_k;&c_s;&c_p;&c_a;&c_n;&c_s;&c_ib;&c_ja;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_w;&c_s;&c_ju;&c_d;&c_zh;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="de">
       <trd>Ausdehnung<klr> (Vorgang)</klr></trd>,
       <trd>Ausweitung</trd>,
       <trd>Erweiterung<klr> (Vorgang)</klr></trd>,
       <trd>Expansion</trd>
    </trdgrp>
    <trdgrp lng="hu">
       <trd>terjeszked&eacute;s</trd>,
       <trd>expanzi&oacute;</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>expansie</trd>,
      <trd>uitbreiding</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>ekspansja</trd>,
      <trd>rozszerzenie</trd>
    </trdgrp>
    <trd lng="pt">expans&atilde;o</trd>
    <trdgrp lng="ru">
      <trd>&c_e;&c_k;&c_s;&c_p;&c_a;&c_n;&c_s;&c_i;&c_ja;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc mrk="ekspan.0o.TEK">
    <uzo tip="fak">TEK</uzo> 
    <dif>
      Dilati&gcirc;o a&ubreve; dispremi&gcirc;o de gaso a&ubreve; de
      vaporo en aparato a&ubreve; ma&scirc;ino: 
      <ekz>
        vaporma&scirc;ino kun triobla <tld/>o.  
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_s;&c_sh;&c_y;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
      <trdgrp lng="de">
         <trd>Ausdehnung</trd>,
         <trd>Expansion</trd>
      </trdgrp>
      <trdgrp lng="hu">
         <trd>kit&aacute;gul&aacute;s</trd>,
         <trd>t&eacute;rfogatn&ouml;veked&eacute;s</trd>
      </trdgrp>
      <trd lng="nl">expansie</trd>
      <trdgrp lng="pl">
        <trd>rozszerzenie</trd>,
        <trd>rozpr&eogonek;&zdot;enie</trd>
      </trdgrp>
      <trd lng="pt">expans&atilde;o</trd>
    <trd lng="ru">&c_r;&c_a;&c_s;&c_sh;&c_i;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <trd lng="ca">expansi&oacute;, expandiment</trd>
  <trdgrp lng="cs">
    <trd>expanze</trd>,
    <trd>p&#x159;&#xED;r&#x16F;stek</trd>,
    <trd>rozp&#xED;navost</trd>,
    <trd>rozp&#xED;n&#xE1;n&#xED;</trd>
  </trdgrp>
  <trd lng="en">expansion</trd>
  <trd lng="es">expansi&oacute;n</trd>
  <trd lng="fr">expansion</trd>
  <trdgrp lng="ja">
    <trd>&#x62E1;&#x5927;  <pr>&#x304B;&#x304F;&#x3060;&#x3044;</pr></trd>,
    <trd>&#x62E1;&#x5F35;  <pr>&#x304B;&#x304F;&#x3061;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x767A;&#x5C55;  <pr>&#x306F;&#x3063;&#x3066;&#x3093;</pr></trd>,
    <trd>&#x81A8;&#x5F35;  <pr>&#x307C;&#x3046;&#x3061;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x5C55;&#x958B;  <pr>&#x3066;&#x3093;&#x304B;&#x3044;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rozp&#xED;navos&#x165;</trd>,
    <trd>tlak zvn&#xFA;tra</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x435;&#x43A;&#x441;&#x43F;&#x430;&#x43D;&#x441;&#x456;&#x44F;</trd>,
    <trd>&#x43F;&#x43E;&#x448;&#x438;&#x440;&#x435;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x440;&#x43E;&#x437;&#x448;&#x438;&#x440;&#x435;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6269;&#x80FD; <pr>ku&#xF2;n&#xE9;ng</pr></trd>,
    <trd>&#x5F20;&#x5F00; <pr>zh&#x101;ngk&#x101;i</pr></trd>,
    <trd>&#x6269;&#x5C55; <pr>ku&#xF2;zh&#x1CE;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ekspan.politika0o">
  <kap>politika <tld/>o</kap>
  <snc mrk="ekspan.politika0o.POL">
    <dif>
      Plivasti&gcirc;o de la povo a&ubreve; de politika influo. 
    </dif>
    <trd lng="be">&c_p;&c_a;&c_l;&c_ib;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_ja; &c_e;&c_k;&c_s;&c_p;&c_a;&c_n;&c_s;&c_ib;&c_ja;</trd>
    <trd lng="ca">expansi&oacute; pol&iacute;tica</trd>
    <trdgrp lng="de">
       <trd>politische Ausdehnung</trd>,
       <trd>politische Expansion</trd>
    </trdgrp>
    <trd lng="en">political expansion</trd>
    <trdgrp lng="hu">
       <trd>politikai <ind>terjeszked&eacute;s</ind></trd>,
       <trd>expanzi&oacute;</trd>
    </trdgrp>
    <trd lng="nl">politieke <ind>expansie</ind></trd>
    <trdgrp lng="pl">
      <trd>ekspansja<klr> polityczna</klr></trd>
      <trd>rozszerzenie<klr> wp&lstroke;yw&oacute;w</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="ja">&#x653F;&#x6CBB;&#x7684;&#x62E1;&#x5927;  <pr>&#x305B;&#x3044;&#x3058;&#x3066;&#x304D;&#x304B;&#x304F;&#x3060;&#x3044;</pr></trd>
</drv>

<drv mrk="ekspan.ekonomia0o">
  <kap>ekonomia <tld/>o</kap>
  <snc num="1">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Ekkonkero de debito en fremdaj landoj. 
    </dif>
    <adm>
      Mi pridubas la limigon de ekonomia ekspansio al io rilate fremdaj
      landoj, la afero same eblas ene de unu lando.<aut>UF</aut>
    </adm>
    <trd lng="be">&c_e;&c_k;&c_a;&c_n;&c_a;&c_m;&c_ib;&c_ch;&c_n;&c_a;&c_ja; &c_e;&c_k;&c_s;&c_p;&c_a;&c_n;&c_s;&c_ib;&c_ja;</trd>
    <trd lng="ca">expansi&oacute; exon&ograve;mica</trd>
    <trdgrp lng="de">
       <trd>wirtschaftliche Ausdehnung</trd>,
       <trd>wirtschaftliche Expansion</trd>
    </trdgrp>
    <trd lng="en">economic expansion</trd>
    <trdgrp lng="hu">
       <trd>gazdas&aacute;gi <ind>terjeszked&eacute;s</ind></trd>,
       <trd>gazdas&aacute;gi <ind>expanzi&oacute;</ind></trd>
    </trdgrp>
    <trd lng="nl">economische <ind>expansie</ind></trd>
    <trd lng="pl">ekspansja ekonomiczna</trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x7D4C;&#x6E08;&#x7684;&#x62E1;&#x5927;  <pr>&#x3051;&#x3044;&#x3056;&#x3044;&#x3066;&#x304D;&#x304B;&#x304F;&#x3060;&#x3044;</pr></trd>,
    <trd>&#x7D4C;&#x6E08;&#x767A;&#x5C55;  <pr>&#x3051;&#x3044;&#x3056;&#x3044;&#x306F;&#x3063;&#x3066;&#x3093;</pr></trd>
  </trdgrp>
</drv>
</art>
