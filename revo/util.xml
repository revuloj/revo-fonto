<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>util</rad>/a
</kap>
<drv mrk="util.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Profite uzebla; produktanta efektive bonan rezultaton:
      <ekz>
        <tld/>a instrumento, libro, institucio, le&gcirc;o;
      </ekz>
      <ekz>
        <tld/>a al iu, por iu a&ubreve; io;
      </ekz>
      <ekz>
        per kio mi povas esti al vi <tld/>a?
      </ekz>
      <ekz>
        kiel granda estas la <tld/>eco de internacia lingvo!
      </ekz>
      <ekz>
        &ccirc;ion por ornamo, nenion por <tld/>eco.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="oportu.0a">oportuna</ref>,
      <ref cel="tauxg.0a">ta&ubreve;ga</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">u&zcaron;ite&ccaron;n&yacute;</trd>
  <trd lng="de">n&uuml;tzlich</trd>
  <trd lng="en">useful</trd>
  <trd lng="es">&uacute;til</trd>
  <trd lng="fr">utile</trd>
  <trd lng="hu">hasznos</trd>
  <trdgrp lng="nl">
    <trd>nuttig</trd>,
    <trd>handig</trd>
  </trdgrp>
  <trd lng="pt">&uacute;til</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_l;&c_je;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_g;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">nyttig</trd>
  <trd lng="sk">u&zcaron;ito&ccaron;n&yacute;</trd>
</drv>

<drv mrk="util.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Io <tld/>a:
      <ekz>
        tiu kondi&ccirc;o donas <tld/>on al la ludonanto;
      </ekz>
      <ekz>
        mi faras tion por via <tld/>o;
      </ekz>
      <ekz>
        li &scirc;atas la belon kaj mal&scirc;atas la <tld/>on.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Nutzen</trd>
  <trd lng="fr">utilit&eacute;</trd>
  <trd lng="hu">haszon</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_mol;&c_z;&c_a;</trd>
  <trd lng="sv">nytta</trd>
</drv>

<drv mrk="util.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>a; liveri <tld/>on:
      <ekz>
        &ccirc;u tio <tld/>us por vi, al vi?
      </ekz>
      <ekz>
        sen&ccirc;esaj admonoj tute ne <tld/>as<fnt>B</fnt>;
      </ekz>
      <ekz>
        ne &ccirc;io <tld/>as, kio brilas
        (<ref tip="sin" cel="frukt.0o">frukto</ref>)
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">n&uuml;tzlich sein</trd>
  <trdgrp lng="hu">
    <trd>haszn&aacute;l<klr tip="ind"> (hasznos)</klr></trd>,
    <trd>hasznos</trd>
  </trdgrp>
  <trd lng="pt">ser &uacute;til</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_y;&c_t;&c_mol; <ind>&c_p;&c_o;&c_l;&c_je;&c_z;&c_n;&c_y;&c_m;</ind></trd>,
    <trd>&c_p;&c_r;&c_i;&c_n;&c_o;&c_s;&c_i;&c_t;&c_mol; <ind>&c_p;&c_o;&c_l;&c_mol;&c_z;&c_u;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="util.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Uzi ion, tiel ke &gcirc;i estu <tld/>a:
      <ekz>
        ne<tld/>igebla rompita&jcirc;o.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">nutzen</trd>
  <trd lng="fr">utiliser</trd>
  <trd lng="hu">hasznos&iacute;t</trd>
  <trd lng="pt">utilizar</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_p;&c_o;&c_l;&c_mol;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_t;&c_i;&c_l;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>nyttja</trd>,
    <trd>anv&auml;nda</trd>
  </trdgrp>
</drv>

<drv mrk="util.0ismo">
  <kap><tld/>ismo</kap>
  <uzo tip="fak">FIL</uzo>
  <snc>
    <dif>
      Doktrino, kiu rigardas la <tld/>on kaj profiton kiel nuran principon
      de konduto.
    </dif>
  </snc>
  <trd lng="de">Utilitarismus</trd>
  <trdgrp lng="hu">
    <trd>haszonelv&udblac;s&eacute;g</trd>,
    <trd>utilitarizmus</trd></trdgrp>
  <trd lng="ru">&c_u;&c_t;&c_i;&c_l;&c_i;&c_t;&c_a;&c_r;&c_i;&c_z;&c_m;</trd>
</drv>

<drv mrk="util.mal0a">
  <kap><ofc>*</ofc>mal<tld/>a</kap>
  <snc>
    <dif>
      Malprofite uzebla; produktanta doma&gcirc;on a&ubreve; difekton:
      <ekz>
        mal<tld/>a birdo, vermo, konsilo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">nutzlos</trd>
  <trd lng="fr">nuisible</trd>
  <trdgrp lng="hu">
    <trd>k&aacute;ros</trd>,
    <trd>&aacute;rtalmas</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_r;&c_je;&c_d;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="util.mal0o">
  <kap>mal<tld/>o</kap>
  <snc>
    <dif>
      Io mal<tld/>a:
      <ekz>
        li fari&gcirc;as tiel potenca nur por nia mal<tld/>o;
      </ekz>
      <ekz>
        ri&ccirc;eco konservata nur por la mal<tld/>o de &gcirc;ia propra
        mastro<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="profit.mal0o">malprofito</ref>,
      <ref cel="difekt.0o">difekto</ref>,
      <ref cel="averi.0o">averio</ref>,
      <ref cel="vund.0o">vundo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Nutzlosigkeit</trd>
  <trd lng="fr">nuisance</trd>
  <trdgrp lng="hu">
    <trd>k&aacute;r</trd>,
    <trd>&aacute;rtalom</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_r;&c_je;&c_d;</trd>
</drv>

<drv mrk="util.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti mal<tld/>a:
      <ekz>
        kiu mal&scirc;atas bonan konsilon, mal<tld/>as al si mem<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &gcirc;i neniom mal<tld/>as.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">nutzlos sein</trd>
  <trd lng="fr">nuire</trd>
  <trdgrp lng="hu">
    <trd>&aacute;rt</trd>,
    <trd>k&aacute;ros</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_r;&c_je;&c_d;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="util.ne0asen0a">
  <kap>ne<tld/>a, sen<tld/>a</kap>
  <snc>
    <dif>
      Produktanta nenian <tld/>on:
      <ekz>
        sen<tld/>a zorgo
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="frukt.sen0a">senfrukta</ref>,
      <ref cel="van.0a">vana</ref>,
      <ref cel="flu.super0a">superflua</ref>,
      <ref cel="balast.0o">balasto</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="drasx.0i">dra&scirc;i</ref>,
      <ref cel="melk.0i">melki</ref>,
      <ref cel="kribr.0ilo">kribrilo</ref>,
      <ref cel="tord.0i">tordi</ref>.
    </refgrp>
  </snc>
  <trd lng="de">unbrauchbar</trd>
  <trd lng="fr">inutile</trd>
  <trd lng="hu">haszontalan</trd>
  <trd lng="pt">in&uacute;til</trd>
  <trd lng="ru">&c_b;&c_je;&c_s;&c_p;&c_o;&c_l;&c_je;&c_z;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
