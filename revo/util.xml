<drv mrk="util.mal0a">
  <kap><ofc>*</ofc>mal<tld/>a</kap>
  <snc>
    <dif>
      Malprofite uzebla; produktanta doma&gcirc;on a&ubreve;
      difekton:
      <ekz>
        mal<tld/>a birdo, vermo, konsilo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_k;&c_o;&c_d;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>neu&zcaron;ite&ccaron;n&yacute;</trd>,
    <trd>obt&ecaron;&zcaron;uj&iacute;c&iacute;</trd>,
    <trd>&scaron;kodliv&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>nachteilig</trd>,
    <trd>abtr&auml;glich</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>harmful</trd>,
    <trd>disadvantageous</trd>
  </trdgrp>
  <trd lng="es">perjudicial</trd>
  <trdgrp lng="fi">
    <trd>haitallinen</trd>,
    <trd>vahingollinen</trd>
  </trdgrp>
  <trd lng="fr">nuisible</trd>
  <trdgrp lng="hu">
    <trd>k&aacute;ros</trd>,
    <trd>&aacute;rtalmas</trd>
  </trdgrp>
  <trd lng="io">desutila</trd>
  <trd lng="nl">nadelig</trd>
  <trd lng="ru">&c_v;&c_r;&c_je;&c_d;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>neu&zcaron;ito&ccaron;n&yacute;</trd>,
    <trd>ob&tcaron;a&zcaron;uj&uacute;ci</trd>,
    <trd>&scaron;kodliv&yacute;</trd>
  </trdgrp>
  <trd lng="tr">zararl&inodot;</trd>
</drv>

<drv mrk="util.mal0o">
  <kap>mal<tld/>o</kap>
  <snc>
    <dif>
      Io mal<tld/>a:
      <ekz>
        li fari&gcirc;as tiel potenca nur por nia mal<tld/>o;
      </ekz><ekz>
        ri&ccirc;eco konservata nur por la mal<tld/>o de &gcirc;ia propra
        mastro<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="profit.mal0o">malprofito</ref>,
      <ref cel="difekt.0o">difekto</ref>,
      <ref cel="averi.0o">averio</ref>,
      <ref cel="vund.0o">vundo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_k;&c_o;&c_d;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>&uacute;hona</trd>,
    <trd>&uacute;jma</trd>,
    <trd>&scaron;koda</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>disadvantage</trd>,
    <trd>detriment</trd>
  </trdgrp>
  <trdgrp lng="fi">
    <trd>haitta</trd>,
    <trd>vahinko</trd>
  </trdgrp>
  <trd lng="fr">nuisance</trd>
  <trdgrp lng="hu">
    <trd>k&aacute;r</trd>,
    <trd>&aacute;rtalom</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_r;&c_je;&c_d;</trd>
  <trd lng="sk">&scaron;koda</trd>
  <trd lng="tr">zararl&inodot;l&inodot;k</trd>
</drv>

<drv mrk="util.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti mal<tld/>a:
      <ekz>
        kiu mal&scirc;atas bonan konsilon, mal<tld/>as al si mem<fnt>Z</fnt>;
      </ekz><ekz>
        &gcirc;i neniom mal<tld/>as.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_k;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>neprosp&iacute;vat</trd>,
    <trd>ubl&iacute;&zcaron;it</trd>,
    <trd>u&scaron;kodit</trd>,
    <trd>zp&uring;sobit &scaron;kodu</trd>,
    <trd>&scaron;kodit</trd>
  </trdgrp>
  <trd lng="de">nachteilig sein</trd>
  <trdgrp lng="fi">
    <trd>tehd&auml; <ind>haittaa</ind></trd>,
    <trd>haitata</trd>
  </trdgrp>
  <trd lng="fr">nuire</trd>
  <trdgrp lng="hu">
    <trd>&aacute;rt</trd>,
    <trd>k&aacute;ros</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_r;&c_je;&c_d;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>u&scaron;kodi&tcaron;</trd>,
    <trd>&scaron;kodi&tcaron;</trd>
  </trdgrp>
  <trd lng="tr">zarar vermek</trd>
</drv>

<drv mrk="util.ne0asen0a">
  <kap>ne<tld/>a, <var><kap>sen<tld/>a</kap></var></kap>
  <snc>
    <dif>
      Produktanta nenian <tld/>on:
      <ekz>
        sen<tld/>a zorgo
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="frukt.sen0a">senfrukta</ref>,
      <ref cel="van.0a">vana</ref>,
      <ref cel="flu.super0a">superflua</ref>,
      <ref cel="balast.0o">balasto</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="drasx.0i">dra&scirc;i</ref>,
      <ref cel="melk.0i">melki</ref>,
      <ref cel="kribr.0ilo">kribrilo</ref>,
      <ref cel="tord.0i">tordi</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_s;&c_k;&c_a;&c_r;&c_y;&c_s;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>marn&yacute;</trd>,
    <trd>neu&zcaron;ite&ccaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>nutzlos</trd>,
    <trd>unbrauchbar</trd>
  </trdgrp>
