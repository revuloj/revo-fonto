<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: preter1.xml,v 1.23 2023/10/20 16:38:17 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>preterit</rad>/o
</kap>
<drv mrk="preter1.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">LIN</uzo>
    <dif>
      Verba <ref tip="super" cel="temp.0o.GRA">tempo</ref>, 
      montranta senprecize la <ref cel="est.0inteco">estintecon</ref>
      (= is-tempo):
      <ekz>
        li devus akcenti, ke li rigardas la aferon per okcidentaj
        okuloj, 
        &ccirc;ar apliko de terminoj kiel prezenco kaj <tld/>o estas
        diskutinda, 
        kiel anka&ubreve; la rifuzo de futuro al la lingvo
        <fnt>
          <bib>Monato</bib>
          <aut>Petro Desmet'</aut>
          <vrk><url ref="&Monato;/2011/010166.php">Klara
          kaj amuza ha&scirc;io</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="futur.0o">futuro</ref>,
      <ref cel="imperf.0o">imperfekto</ref>,
      <ref cel="perfek1.0o">perfekto</ref>,
      <ref cel="prezen.0o">prezenco</ref>,
      <ref cel="pluskv.0o">pluskvamperfekto</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_o;&c_sh;&c_l;&c_y; &c_ch;&c_a;&c_s;</trd>
  <trdgrp lng="cs">
    <trd>minul&yacute; &ccaron;as</trd>,
    <trd>pr&eacute;teritum</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Vergangenheitsform</trd>,
    <trd>Pr&auml;teritum</trd>
  </trdgrp>
  <trd lng="en">past tense</trd>
  <trd lng="es">pret&eacute;rito</trd>
  <trd lng="fr">pr&eacute;t&eacute;rit</trd>
  <trd lng="hu">m&uacute;lt <ind>id&odblac;</ind></trd>
  <trd lng="ja">&#x904E;&#x53BB;  <pr>&#x304B;&#x3053;</pr></trd>
  <trdgrp lng="nl">
    <trd>praeteritium</trd>,
    <trd>verleden <ind>tijd</ind></trd>
  </trdgrp>
  <trd lng="pl">czas przesz&lstroke;y</trd>
  <trd lng="pt">pret&eacute;rito</trd>
  <trd lng="ro">trecut</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_sh;&c_je;&c_d;&c_sh;&c_je;&c_je; 
&c_v;&c_r;&c_je;&c_m;&c_ja;</trd>
  <trd lng="sk">minul&yacute; &ccaron;as</trd>
  <trd lng="uk">&#x43C;&#x438;&#x43D;&#x443;&#x43B;&#x438;&#x439; &#x447;&#x430;&#x441; &#x434;&#x456;&#x454;&#x441;&#x43B;&#x43E;&#x432;&#x430;</trd>
</drv>
</art>
