<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>preterit</rad>/o
</kap>
<drv mrk="preter1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Verba tempo, montranta senprecize la estintecon (= is-tempo).
    </dif>
    <refgrp tip="sin">
      <ref cel="futur.0o">futuro</ref>,
      <ref cel="imperf.0o">imperfekto</ref>,
      <ref cel="perfek.0o">perfekto</ref>,
      <ref cel="prezen.0o">prezenco</ref>,
      <ref cel="pluskv.0o">pluskvamperfekto</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Vergangenheit</trd>
  <trdgrp lng="nl">
    <trd>praeteritium</trd>,
    <trd>verleden <ind>tijd</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_sh;&c_je;&c_d;&c_sh;&c_je;&c_je; &c_v;&c_r;&c_je;&c_m;&c_ja;</trd>
</drv>
</art>
