<vortaro>
<art mrk="$Id: grupet.xml,v 1.9 2019/03/17 10:10:14 revo Exp $">
<kap><rad>grupet</rad>/o <fnt><bib>NePiv</bib></fnt></kap>
<drv mrk="grupet.0o">
  <kap><tld/>o</kap>
  <!--mlg>&#119191;</mlg -->
  <snc>
    <dif>
      <ref tip="super" cel="ornam.melodi0o">Melodiornamo</ref>
      uzata en klasika e&ubreve;ropa muziko,
      kiu alternigas la tonon sinsekve unufoje kun la supra
      kaj malsupra tonoj, simboligita per spegulita ku&scirc;anta &leftquot;S&rightquot;:
      <ekz>
        inversan <tld/>on oni komencas je la malsupra tono kaj simboligas per ku&scirc;anta &leftquot;S&rightquot;
        <fnt>
        </fnt>.
      </ekz>      
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Doppelschlag</trd>,
    <trd>Grupetto</trd>
  </trdgrp>
  <trd lng="es">grupeto</trd>
  <trdgrp lng="fr">
    <trd>grupetto</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>cifr&aacute;zat</trd>,
    <trd>&eacute;k&iacute;t&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>grupetto</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>grupetto</trd>
  </trdgrp>
  <trd lng="pt">grupeto</trd>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_p;&c_je;&c_t;&c_t;&c_o;</trd>
</drv>
</art>
