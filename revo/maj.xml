  <trd lng="ja">&#20116;&#26376;&#12398; <pr>&#12372;&#12364;&#12388;&#12398;</pr></trd>
  <trd lng="pl">majowy</trd>
  <trd lng="uk">&c_M;&c_a;&c_j;&c_ja; (&c_n;&c_ib;&c_m;&c_f;&c_a; &c_g;&c_ib;&c_r;)</trd>
