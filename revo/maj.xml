<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>maj</rad>/o
</kap>
<drv mrk="maj.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Kvina monato de la jaro. 
    </dif>
  </snc>
  <ref tip="vid" cel="januar.0o">januaro, Rim.</ref>
  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_a;&c_v;&c_je;&c_n;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_j;</trd>
  </trdgrp>
  <trd lng="cs">kv&ecaron;ten</trd>
  <trd lng="de">Mai</trd>
  <trd lng="en">May</trd>
  <trd lng="fr">mai</trd>
  <trd lng="hu">m&aacute;jus</trd>
  <trd lng="nl">mei</trd>
  <trd lng="pl">maj</trd>
  <trd lng="ru">&#1084;&#1072;&#1081;</trd>
  <trd lng="sv">maj</trd>
  <trd lng="tr">mayis</trd>
</drv>
<drv mrk="maj.0floro">
  <kap><tld/>floro</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="konval.0o">Konvalo</ref>. 
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_n;&c_d;&c_y;&c_sh;</trd>
  <trd lng="de">Maigl&ouml;ckchen</trd>
  <trd lng="hu">gy&ouml;ngyvir&aacute;g</trd>
  <trdgrp lng="nl">
    <trd>meiklokje</trd>,
    <trd>lelietje-van-dalen</trd>
  </trdgrp>
</drv>
<drv mrk="maj.0skarabo">
  <kap><tld/>skarabo</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="melolo.0o">Melolonto</ref>.  
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_r;&c_u;&c_sh;&c_ch;</trd>
  <trd lng="de">Maik&auml;fer</trd>
  <trd lng="hu">cserebog&aacute;r</trd>
  <trd lng="nl">meikever</trd>
  <trd lng="pl">chrab&aogonek;szcz majowy</trd>
</drv>
</art>
