<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>maj</rad>/o
</kap>
<drv mrk="maj.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Kvina monato de la jaro. 
    </dif>
  </snc>
  <ref tip="vid" cel="januar.0o">januaro, Rim.</ref>
  <trd lng="cs">kv&ecaron;ten</trd>
  <trd lng="de">Mai</trd>
  <trd lng="en">May</trd>
  <trd lng="fr">mai</trd>
  <trd lng="nl">mei</trd>
  <trd lng="pl">maj</trd>
</drv>
<drv mrk="maj.0floro">
  <kap><tld/>floro</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="konval.0o">Konvalo</ref>. 
    </dif>
  </snc>
  <trd lng="de">Maigl&ouml;ckchen</trd>
  <trd lng="nl">meiklokje</trd>
</drv>
<drv mrk="maj.0skarabo">
  <kap><tld/>skarabo</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="melolont.0o">Melolonto</ref>.  
    </dif>
  </snc>
  <trd lng="de">Maik&auml;fer</trd>
  <trd lng="nl">meikever</trd>
  <trd lng="pl">chrab&aogonek;szcz majowy</trd>
</drv>
</art>
