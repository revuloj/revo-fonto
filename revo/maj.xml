<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: maj.xml,v 1.42 2021/05/28 12:53:01 revo Exp $">
<kap>
  <ofc>*</ofc> <rad>Maj</rad>/o,
  <var><kap><rad var="m">maj</rad>/o</kap></var>
</kap>
<drv mrk="maj.0o">
  <kap><ofc>*</ofc><tld/>o, <var><kap><tld var="m"/>o</kap></var></kap>
  <snc mrk="maj.0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Kvina <ref tip="lst" cel="monat.0o.KAL" lst="voko:monatoj" val="5">monato</ref> de la <ref tip="malprt" cel="jar.0o.KAL">jaro</ref> la&ubreve; la Gregoria kalendaro, inter
      <ref cel="april.0o">Aprilo</ref> kaj <ref cel="juni.0o">Junio</ref>:
      <ekz>
        hodia&ubreve; estas &jcirc;a&ubreve;do la 8 de <tld/>o de la jaro 1890
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la unuan de <tld/>o de la sekvanta jaro ili revenis
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        de oktobro &gcirc;is la fino de <tld lit="m"/>o de sekvanta jaro la akvoj
        mallevi&gcirc;as
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld lit="m"/>o, <tld lit="m"/>o, <tld lit="m"/>monato, / amatino kun amato
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="http://steloj.de/esperanto/butiko">La Stranga
          Butiko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="januar.0o.RIM">Januaro, Rim.</ref>
  <trd lng="af">Mei</trd>
  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_a;&c_v;&c_je;&c_n;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_j;</trd>
  </trdgrp>
  <trd lng="bg">&c_m;&c_a;&c_j;</trd>
  <trd lng="bo">&#xF5F;&#xFB3;&#xF0B;&#xF56;&#xF0B;&#xF63;&#xF94;&#xF0B;&#xF54;&#xF0B;</trd>
  <trd lng="br">Mae</trd>
  <trd lng="ca">maig</trd>
  <trd lng="cs">kv&ecaron;ten</trd>
  <trdgrp lng="de">
    <trd>Mai</trd>,
    <trd>Wonnemonat<klr> [kromnomo]</klr></trd>,
    <trd>Wonnemond<klr> [ark]</klr></trd>
  </trdgrp>
  <trd lng="en">May</trd>
  <trd lng="es">mayo</trd>
  <trd lng="fr">mai</trd>
  <trd lng="fy">maaie</trd>
  <trd lng="he">&mem;&alef;&yod;</trd>
  <trd lng="hu">m&aacute;jus</trd>
  <trd lng="id">Mei</trd>
  <trd lng="io">mayo</trd>
  <trd lng="it">maggio</trd>
  <trd lng="ja">&#x4E94;&#x6708; [<ind>&#x3054;&#x304C;&#x3064;</ind>]</trd>
  <trdgrp lng="jbo">
    <trd>mumyma'i</trd>,
    <trd>bakma'i</trd>
  </trdgrp>
  <trd lng="nl">mei</trd>
  <trd lng="pl">maj</trd>
  <trd lng="pt">maio</trd>
  <trd lng="ro">mai</trd>
  <trd lng="ru">&c_m;&c_a;&c_j;</trd>
  <trd lng="sk">m&aacute;j</trd>
  <trd lng="sv">maj</trd>
  <trd lng="tp">tenpo mun nanpa luka</trd>
  <trd lng="tr">mayis</trd>
  <trd lng="vo">mayul</trd>
</drv>

<drv mrk="maj.0a">
  <kap><tld lit="m"/>a</kap>
  <snc>
    <dif>
      Okazanta en <tld/>o, rilata al <tld/>o:
      <ekz>
        en la <tld lit="m"/>a numero aperis opinio pri la internacia fonetika
        alfabeto
        <fnt>
          <bib>Monato</bib>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url ref="https://www.monato.be/2003/008148.html">Fonetika
          alfabeto havas alian rolon</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ne pretis la <tld lit="m"/>a konstitucio nur pro manko de interkonsento
        <fnt>
          <bib>Monato</bib>
          <aut>Navin Shrestha/pg</aut>
          <vrk><url ref="&Monato;/2012/010752.php">Fiasko
          konstitucia</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la Popola Partio refoje ricevis absolutan plimulton en la
        <tld lit="m"/>aj elektoj al la baleara parlamento
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2011/010471.php">Koruptado
          demisiigas prezidanton</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="io">mayala</trd>
  <trd lng="ja">&#x4E94;&#x6708;&#x306E; [<ind>&#x3054;&#x304C;&#x3064;&#x306E;</ind>]</trd>
  <trd lng="vo">mayulik</trd>
</drv>
</art>
