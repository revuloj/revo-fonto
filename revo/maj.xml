<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>maj</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="maj.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="maj.0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Kvina monato de la jaro la&ubreve; la gregoria kalendaro.
    </dif>
