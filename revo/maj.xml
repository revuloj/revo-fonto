<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: maj.xml,v 1.40 2019/06/30 18:21:47 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>maj</rad>/o
</kap>
<drv mrk="maj.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">KAL</uzo>
    <dif>
      Kvina <ref tip="lst" cel="monat.0o.KAL" lst="voko:monatoj"
      val="5">monato</ref> de la <ref tip="malprt"
      cel="jar.0o.KAL">jaro</ref> la&ubreve; la Gregoria kalendaro, inter
      aprilo kaj junio:
      <ekz>
        hodia&ubreve; estas &jcirc;a&ubreve;do la 8 de <tld lit="M"/>o de la jaro 1890
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la unuan de <tld lit="M"/>o de la sekvanta jaro ili revenis
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        de oktobro &gcirc;is la fino de <tld/>o de sekvanta jaro la akvoj
        mallevi&gcirc;as
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o, <tld/>o, <tld/>monato, / amatino kun amato
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="http://steloj.de/esperanto/butiko">La Stranga
          Butiko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="januar.0o.RIM">januaro, Rim.</ref>
  <trd lng="af">Mei</trd>
  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_a;&c_v;&c_je;&c_n;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_j;</trd>
  </trdgrp>
  <trd lng="bg">&c_m;&c_a;&c_j;</trd>
  <trd lng="br">Mae</trd>
  <trd lng="ca">maig</trd>
  <trd lng="cs">kv&ecaron;ten</trd>
  <trdgrp lng="de">
    <trd>Mai</trd>,
    <trd>Wonnemonat<klr> [kromnomo]</klr></trd>,
    <trd>Wonnemond<klr> [ark]</klr></trd>
  </trdgrp>
  <trd lng="en">May</trd>
  <trd lng="es">mayo</trd>
  <trd lng="fr">mai</trd>
  <trd lng="fy">maaie</trd>
  <trd lng="he">&mem;&alef;&yod;</trd>
  <trd lng="hu">m&aacute;jus</trd>
  <trd lng="id">Mei</trd>
  <trd lng="io">mayo</trd>
  <trd lng="it">maggio</trd>
  <trd lng="ja">&#20116;&#26376; [<ind>&#12372;&#12364;&#12388;</ind>]</trd>
  <trdgrp lng="jbo">
    <trd>mumyma'i</trd>,
    <trd>bakma'i</trd>
  </trdgrp>
  <trd lng="nl">mei</trd>
  <trd lng="pl">maj</trd>
  <trd lng="pt">maio</trd>
  <trd lng="ro">mai</trd>
  <trd lng="ru">&c_m;&c_a;&c_j;</trd>
  <trd lng="sk">m&aacute;j</trd>
  <trd lng="sv">maj</trd>
  <trd lng="tp">tenpo mun nanpa luka</trd>
  <trd lng="tr">mayis</trd>
  <trd lng="vo">mayul</trd>
</drv>

<drv mrk="maj.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Okazanta en <tld/>o, rilata al <tld/>o:
      <ekz>
        en la <tld/>a numero aperis opinio pri la internacia fonetika
        alfabeto
        <fnt>
          <bib>Monato</bib>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url
          ref="https://www.monato.be/2003/008148.html">Fonetika
          alfabeto havas alian rolon</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ne pretis la <tld/>a konstitucio nur pro manko de interkonsento
        <fnt>
          <bib>Monato</bib>
          <aut>Navin Shrestha/pg</aut>
          <vrk><url ref="&Monato;/2012/010752.php">Fiasko
          konstitucia</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la Popola Partio refoje ricevis absolutan plimulton en la
        <tld/>aj elektoj al la baleara parlamento
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url
          ref="&Monato;/2011/010471.php">Koruptado
          demisiigas prezidanton</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="io">mayala</trd>
  <trd lng="ja">&#20116;&#26376;&#12398; [<ind>&#12372;&#12364;&#12388;&#12398;</ind>]</trd>
  <trd lng="vo">mayulik</trd>
</drv>
</art>
