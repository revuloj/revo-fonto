<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>maj</rad>/o
</kap>
<drv mrk="maj.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Kvina monato de la jaro.
    </dif>
  </snc>
  <ref tip="vid" cel="januar.0o">januaro, Rim.</ref>
  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_a;&c_v;&c_je;&c_n;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_j;</trd>
  </trdgrp>
  <trd lng="cs">kv&ecaron;ten</trd>
  <trdgrp lng="de">
     <trd>Mai</trd>,
     <trd>Wonnemonat<klr> [kromnomo]</klr></trd>,
     <trd>Wonnemond<klr> [ark]</klr></trd>
  </trdgrp>
  <trd lng="br">Mae</trd>
  <trd lng="en">May</trd>
  <trd lng="fr">mai</trd>
  <trd lng="he">&mem;&alef;&yod;</trd>
  <trd lng="hu">m&aacute;jus</trd>
  <trd lng="nl">mei</trd>
  <trd lng="pl">maj</trd>
  <trd lng="ro">mai</trd>
  <trd lng="ru">&c_m;&c_a;&c_j;</trd>
  <trd lng="sv">maj</trd>
  <trd lng="tr">mayis</trd>
  <trd lng="vo">mayul</trd>
</drv>

<drv mrk="maj.0floro">
  <kap>
    <tld/>floro
  </kap>
  <snc>
    <dif>
      <ref tip="dif" cel="konval.0o">Konvalo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_n;&c_d;&c_y;&c_sh;</trd>
  <trd lng="br">louzaouenn-an-ha&ntilde;v</trd>
  <trd lng="de">Maigl&ouml;ckchen</trd>
  <trd lng="fr">muguet</trd>
  <trd lng="hu">gy&ouml;ngyvir&aacute;g</trd>
  <trdgrp lng="nl">
    <trd>meiklokje</trd>,
    <trd>lelietje-van-dalen</trd>
  </trdgrp>
  <trd lng="pl">konwalia</trd>
  <trd lng="ru">&c_l;&c_a;&c_n;&c_d;&c_y;&c_sh;</trd>
</drv>
</art>
