<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kompoz1.xml,v 1.23 2023/12/03 09:58:54 revo Exp $">
<kap>
  <rad>kompozit</rad>/oj <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kompoz1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="kompoz1.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="dif" cel="astera.0oj">Asteracoj</ref>:
      <ekz>
        &ccirc;e iuj <tld lit="K"/>oj, la masklaj floretoj,
        kiuj kompreneble estas nefekundaj, havas
        pistilon kiu estas en rudimenta stato
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 13a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="kompoz1.0oj.RIM_HOM">
    <ref tip="hom" cel="kompozit.0o">Kompozito</ref> estas
    anka&ubreve; nomo de materialo el fibro kaj plasto.
  </rim>
   <trdgrp lng="be">
    <trd>&c_a;&c_s;&c_t;&c_r;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#x2019;&c_ja;)</klr></trd>,
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_a;&c_n;&c_a;&c_k;&c_v;&c_je;&c_t;&c_n;&c_y;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>compostes</trd>,
    <trd>aster&agrave;cies</trd>
  </trdgrp>
  <trd lng="de">Korbbl&uuml;tler</trd>
  <trd lng="fr">compos&eacute;es</trd>
  <trdgrp lng="hu">
    <trd>f&eacute;szkesvir&aacute;gzat&uacute;ak</trd>,
    <trd>aszterf&eacute;l&eacute;k</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>composieten</trd>,
    <trd>samengesteldbloemigen</trd>
  </trdgrp>
  <trd lng="pl">astrowate</trd>
  <trd lng="pt">compostas</trd>
  <trd lng="ro">compositae</trd>
  <trd lng="ru">&c_s;&c_l;&c_o;&c_zh;&c_n;&c_o;&c_c;&c_v;&c_je;&c_t;&c_n;&c_y;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>hviezdicovit&eacute;</trd>,
    <trd>zlo&zcaron;nokvet&eacute;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hv&ecaron;zdicovit&eacute;</trd>,
    <trd>slo&zcaron;nokv&ecaron;t&eacute; <klr>(&ccaron;ele&dcaron;)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&c_j;&c_s;&c_t;&c_r;&#x43E;&c_v;&c_ib;</trd>,
    <trd>&c_s;&#x43A;&#x43B;&c_a;&c_d;&#x43D;&#x43E;&c_c;&c_v;&c_ib;&c_t;&c_ib;</trd>
  </trdgrp>
  <trd lng="zh">&#x7D2B;&#x83C0;&#x5C5E;&#x690D;&#x7269; <pr>z&#x1D0;w&#x1CE;nsh&#x1D4;zh&#xED;w&#xF9;</pr></trd>
</drv>
</art>
