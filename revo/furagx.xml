<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>fura&gcirc;</rad>/o
</kap>
<drv mrk="furagx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      &Ccirc;iu rikoltita herba vegeta&jcirc;o, verda a&ubreve; sekigita, servanta por nutri brutojn.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_o;&c_r;&c_m;</trd>,
    <trd>&c_f;&c_u;&c_r;&c_a;&c_zh;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Tierfutter</trd>,
     <trd>Viehfutter</trd></trdgrp>
  <trdgrp lng="en">
    <trd>forage</trd>,
    <trd>fodder</trd>
  </trdgrp>
  <trd lng="es">forraje</trd>
  <trd lng="fr">fourrage</trd>
  <trd lng="hu">takarm&aacute;ny</trd>
  <trd lng="nl">voeder</trd>
  <trd lng="pt">forragem</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_r;&c_m;</trd>,
    <trd>&c_f;&c_u;&c_r;&c_a;&c_zh;</trd>
  </trdgrp>
</drv>
</art>
