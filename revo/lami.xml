<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>lami</rad>/o
</kap>
<drv mrk="lami.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="lami.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Lamium</trd>), tre ofta en E&ubreve;ropo.   
    </dif>
  </snc>
  <bld lok="../bld/lami.jpg">makulita <tld/>o 
    (<trd lng="la">Lamium maculatum</trd>)</bld>
  <trd lng="de">Taubnessel</trd>
</drv>
<drv mrk="lami.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj, unupetalaj plantoj, 
      kies koroloj havas formon de labio. 
    </dif>
  </snc>
  <trd lng="de">Lippenbl&uuml;tengew&auml;chse</trd>
</drv>
</art>
