<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>lami</rad>/o
</kap>
<drv mrk="lami.0o">
  <kap>
    <tld/>o
  </kap>

  <snc mrk="lami.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio 
      <ref tip="super" cel="lamiac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Lamium</trd>), tre ofta en E&ubreve;ropo.
    </dif>

  <bld lok="../bld/lami.jpg">
    <ind>makulita <tld/>o</ind>
    <trd lng="la">Lamium maculatum</trd>
  </bld>
  <trd lng="be">&c_ja;&c_s;&c_n;&c_o;&c_t;&c_k;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>ortiga blanca</trd>,
    <trd>ortiga borda</trd>,
    <trd>ortiga morta</trd>
  </trdgrp>
  <trd lng="de">Taubnessel</trd>
  <trd lng="en">nettles</trd>
  <trd lng="hu">&aacute;rvacsal&aacute;n</trd>  
  <trd lng="nl">dovenetel</trd>
  <trd lng="pt">l&acirc;mio</trd>
  <trd lng="ru">&c_ja;&c_s;&c_n;&c_o;&c_t;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="lami.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
En multaj mitologioj, virinforma esta&jcirc;o, eliranta nokte por su&ccirc;i
sangon,
abortigi gravedajn virinojn, mortigi infanojn kaj kelkfoje virojn.
    </dif>
    <ref tip="vid" cel="lilit.0o.MIT">Lilito</ref>
  <trd lng="ca">l&agrave;mia</trd>
  <trd lng="de">Lamia</trd>
  <trd lng="it">lamia</trd>
  <trd lng="nl">lamia</trd>
  </snc>
</drv>

<drv mrk="lami.0o.PERS">
  <kap>
    <tld lit="L"/>o
  </kap>
  <snc>
    <uzo tip="fak">MIT</uzo>
    <dif>
      Amantino de 
      <ref tip="vid" cel="zeuxs.0o">Ze&ubreve;so</ref>, transformi&gcirc;ita en
      envia kaj
      sendorma infanmortiga 
      <ref tip="vid" cel="monstr.0o.BIO">monstro</ref> pro la doloro por
      la
      mortigo de siaj infanoj.
    </dif>
    <ref tip="vid" cel="lilit.0o.REL">Lilito</ref>
  </snc>
  <trd lng="ca">L&agrave;mia</trd>
  <trd lng="de">Lamia</trd>
  <trd lng="it">Lamia</trd>
  <trd lng="nl">Lamia</trd>
</drv>
</art>
