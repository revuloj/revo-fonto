<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc> <rad>urs</rad>/o
</kap>
<drv mrk="urs.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="urs.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="ursed.0oj"><tld/>edoj</ref>
      (<trd lng="la">Ursus</trd>),
      plumpa rabobesto kun mallonga dika kolo,
      kvinfingra, plandmar&scirc;anta, kun tre mallonga vosto:
      <ekz>
        murmuregas la <tld/>o sed danci &gcirc;i devas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        fari al iu <tld/>an <klr>(mallertan)</klr> kareson
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>anta&ubreve; mortigo de <tld/>o ne vendu &gcirc;ian felon</ind>
        <trd lng="fr">il ne faut pas <ind>vendre la peau de l'ours avant
        de l'avoir tu&eacute;</ind></trd>
        <klr>(atendu la venkon anta&ubreve; ol fanfaroni)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        fari al iu <tld/>an helpon <klr>(dan&gcirc;eran pro mallerto,
        kiel la <tld/>o, kiu por elpeli mu&scirc;on de sia dormanta
        amiko, frakasis per &scirc;tono lian kapon kune kun la mu&scirc;o)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ja;&c_d;&c_z;&c_mol;&c_v;&c_je;&c_d;&c_z;&c_mol;</trd>
  <trd lng="br">arzh</trd>
  <trd lng="cs">medv&ecaron;d</trd>
  <trd lng="de">B&auml;r</trd>
  <trd lng="el">&alfa;&rho;&kappa;&omikron;&ypsilon_ton;&delta;&alfa;</trd>
  <trd lng="en">bear</trd>
  <trd lng="es">oso</trd>
  <trd lng="fr">ours</trd>
  <trd lng="he">&dalet;&bet;</trd>
  <trd lng="hu">medve</trd>
  <trd lng="it">orso</trd>
  <trd lng="nl">beer</trd>
  <trd lng="pt">urso</trd>
  <trd lng="ru">&c_m;&c_je;&c_d;&c_v;&c_je;&c_d;&c_mol;</trd>
  <trd lng="sv">bj&ouml;rn</trd>
  <trd lng="tr">ay&inodot;</trd>
</drv>

<drv mrk="urs.GrandaU0ino">
  <kap>Granda <tld lit="U"/>ino</kap>
  <snc mrk="urs.GrandaU0ino.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Granda &ccirc;erpilforma stelaro klare videbla en la norda hemisfero:
      <ekz>
        malproksime, vualita de nebulo, briletis Nilo, supre brilis la sep
        steloj de la Granda <tld lit="U"/>ino
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 20a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">Grande ourse</trd>
  <trd lng="la">Ursa Major</trd>
  <trd lng="ca">Ossa Major</trd>
  <trdgrp lng="cs">
    <trd>Velk&aacute; Medv&ecaron;dice</trd>,
    <trd>Velk&yacute; Medv&ecaron;d</trd>,
    <trd>Velk&yacute; V&uring;s</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gro&szlig;er B&auml;r</trd>,
    <trd>Gro&szlig;er Wagen</trd>
  </trdgrp>
  <trd lng="el">&My;&epsilon;&gamma;&alfa_ton;&lambda;&eta; &Alfa_ton;&rho;&kappa;&tau;&omikron;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>Greater Bear</trd>,
    <trd>Great Bear</trd>
  </trdgrp>
  <trd lng="es">Osa Mayor</trd>
  <trdgrp lng="fr">
    <trd>Grand charriot</trd>,
    <trd>Grande ourse</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&ayin;&gimel;&lamed;&he; &gimel;&dalet;&vav;&lamed;&he; Agala gdola</trd>,
    <trd>&dalet;&vav;&bet;&he; &gimel;&dalet;&vav;&lamed;&he; Duba gdola</trd>
  </trdgrp>
  <trd lng="hu">Nagy Medve</trd>
  <trdgrp lng="it">
    <trd>Orsa Maggiore</trd>,
    <trd>Grande Carro</trd>
  </trdgrp>
  <trd lng="nl">Grote Beer</trd>
  <trd lng="pl">Wielka Nied&zacute;wiedzica</trd>
  <trd lng="pt">Ursa Maior</trd>
  <trd lng="ru">&c_B;&c_o;&c_l;&c_o;&c_mol;&c_sh;&c_a;&c_ja; &c_M;&c_je;&c_d;&c_v;&c_je;&c_d;&c_i;&c_c;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>Ve&lcaron;k&yacute; voz</trd>,
    <trd>Ve&lcaron;k&aacute; medvedica</trd>
  </trdgrp>
  <trdgrp lng="sl">
    <trd>Veliki medved</trd>,
    <trd>Veliki voz</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>B&uuml;y&uuml;k Ay&inodot;</trd>,
    <trd>B&uuml;y&uuml;kay&inodot;</trd>
  </trdgrp>
</drv>

<drv mrk="urs.MalgrandaU0ino">
  <kap>Malgranda <tld lit="U"/>ino</kap>
  <snc mrk="urs.MalgrandaU0ino.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      La norda <ref tip="super" cel="stel.0aro">stelaro</ref>
      &ccirc;erpilforma, en kiu la ekstrema
      stelo de la &ccirc;erpiltenilo estas la <ref tip="prt"
      cel="polus.P0astelo">Polusa stelo</ref>.
    </dif>
    <trd lng="br">Kastell-karr-kamm bihan</trd>
    <trdgrp lng="cs">
      <trd>Mal&yacute; Medv&ecaron;d</trd>,
      <trd>Mal&yacute; V&uring;z</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>Little Bear</trd>,
      <trd>Little Dipper</trd>,
      <trd>Ursa Minor</trd>
    </trdgrp>
    <trd lng="es">Osa Menor</trd>
    <trdgrp lng="fr">
      <trd>Petit charriot</trd>,
      <trd>Petite ourse</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>Kismedve</trd>,
      <trd>Kisg&ouml;nc&ouml;l</trd>
    </trdgrp>
    <trdgrp lng="he">
      <trd>&he;&dalet;&vav;&bet;&he; &he;&qof;&tet;&nun;&he;</trd>,
      <trd>&he;&ayin;&gimel;&lamed;&he; &he;&qof;&tet;&nun;&he;</trd>
    </trdgrp>
    <trd lng="la">Ursa Minor</trd>
    <trd lng="pt">Ursa Menor</trd>
    <trd lng="ru">&c_M;&c_a;&c_l;&c_a;&c_ja; &c_M;&c_je;&c_d;&c_v;&c_je;&c_d;&c_i;&c_c;&c_a;</trd>
  </snc>
</drv>
</art>
