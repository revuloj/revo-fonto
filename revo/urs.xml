<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>urs</rad>/o
</kap>
<drv mrk="urs.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="urs.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Ursus</trd>),
      plumpa rabobesto kun mallonga dika kolo,
      kvinfingra, plandmar&scirc;anta, kun tre mallonga vosto:
      <ekz>
        murmuregas la <tld/>o sed  danci &gcirc;i devas<fnt>Z</fnt>;
      </ekz>
      <ekz>
        fari al iu <tld/>an (mallertan) kareson<fnt>Z</fnt>;
      </ekz>
      <ekz>
        anta&ubreve; mortigo de <tld/>o ne vendu &gcirc;ian felon<fnt>Z</fnt>
        <klr>(atendu la venkon anta&ubreve; ol fanfaroni:
        <refgrp tip="vid">
          <ref cel="fru">tro frue</ref>,
          <ref cel="triumf.0i">triumfi</ref>
        </refgrp>
        )</klr>;
      </ekz>
      <ekz>
        fari al iu <tld/>an helpon <klr>(dan&gcirc;eran pro mallerto,
        kiel la <tld/>o, kiu por elpeli mu&scirc;on de sia dormanta amiko,
        frakasis
        per &scirc;tono lian kapon kune kun la mu&scirc;o)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">B&auml;r</trd>
  <trd lng="en">bear</trd>
  <trd lng="fr">ours</trd>
  <trd lng="nl">beer</trd>
  <trd lng="ru">&c_m;&c_je;&c_d;&c_v;&c_je;&c_d;&c_mol;</trd>
  <trd lng="tr">ay&inodot;</trd>
</drv>
<drv mrk="urs.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super"
      cel="best.rabo0oj">rabobestoj</ref>
      (<trd lng="la">Ursidae</trd>).
    </dif>
  </snc>
  <trd lng="de">B&auml;ren <klr tip="ind">(Familie)</klr></trd>
  <trd lng="fr">ursid&eacute;s</trd>
</drv>
</art>
