<drv mrk="urs.blanka0o">
  <kap>blanka <tld/>o</kap>
  <snc mrk="urs.blanka0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="urs.0o" lst="voko:rabobestoj"><tld lit="U"/>o</ref>
       kun blanka &gcirc;is kremkolora hararo;
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:zoologiaj_specioj">specio</ref> vivanta en Arkto
      (<trd lng="la">Ursus maritimus</trd>):
      <ekz>
        67-jara viro rakontis, ke li travivis atakon de
        blanka <tld/>o en Nunavuto, Kanado
        <fnt>
          <aut>Arono kaj Karlina</aut>,
          <vrk><url ref="http://radioverda.com/">
          Radio Verda</url></vrk>,
          <lok>rv171, 2010-09-04</lok>
        </fnt>.
      </ekz>
    </dif>    
  </snc>
  <trd lng="be">&c_b;&c_je;&c_l;&c_y; &c_m;&c_ja;&c_d;&c_z;&c_mol;&c_v;&c_je;&c_d;&c_z;&c_mol;</trd>
  <trd lng="bg">&c_b;&c_ja;&c_l;&c_a; &c_m;&c_je;&c_ch;&c_k;&c_a;</trd>
  <trd lng="cs">pol&aacute;rn&iacute; medv&ecaron;d</trd>
  <trd lng="de">Eisb&auml;r</trd>
  <trd lng="en">polar bear</trd>
  <trdgrp lng="fr">
    <trd>ours blanc</trd>,
    <trd>ours polaire</trd>
  </trdgrp>
  <trd lng="id"><ind>beruang</ind> kutub</trd>
  <trd lng="it">orso<klr> polare</klr></trd>
  <trd lng="ja">&#x30B7;&#x30ED;&#x30AF;&#x30DE;</trd>
  <trd lng="nl">ijsbeer</trd>
  <trdgrp lng="pl">
    <trd>nied&zacute;wied&zacute; polarny</trd>,
    <trd>bia&lstroke;y <ind>nied&zacute;wied&zacute;</ind></trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>urs polar</trd>,
    <trd>urs alb</trd>
  </trdgrp>
  <trd lng="sk">pol&aacute;rny medve&dcaron;</trd>
</drv>

<drv mrk="urs.lav0o">
  <kap>lav<tld/>o, <var><kap>lav<tld/>eto</kap></var></kap>
  <snc mrk="urs.lav0o.ZOO">
     <uzo tip="fak">ZOO</uzo>
     <dif>
       Hunde granda <ref tip="lst" cel="best.rabo0o" lst="voko:rabobestoj">rabobesto</ref> el Nord-
       kaj Centrameriko,
       kun griza felo, nigra-blankaj strioj sur la kapo kaj la vosto, 
       enakviganta nutra&jcirc;ojn, anta&ubreve; ol &gcirc;i man&gcirc;as ilin;
       <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref>
       el la familio 
       <ref tip="malprt" cel="procioned.0oj">procionedoj</ref>
       (<trd lng="la">Procyon</trd>):
       <ekz>
         sur la strato mi vidis lav<tld/>on
         <fnt>
           <aut>vik1887</aut>,
           <vrk><url ref="http://vik1887.livejournal.com/">
           Vik en Esperantolando</url></vrk>,
           <lok> 2005-12-28</lok>
         </fnt>;
       </ekz>
       <ekz>
         lav<tld/>eto <klr>...</klr> anka&ubreve; nomi&gcirc;as prociono
         <fnt>
           <aut>T. Alexander</aut>,
           <vrk> Besto- kaj planto-nomoj</vrk>,
           <lok><url ref="http://groups.google.com/group/bablo/">
           bablo</url>, 2007-10-05</lok>
         </fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_ja;&c_n;&c_o;&c_t;</trd>
   <trd lng="bg">&c_m;&c_i;&c_je;&c_shch;&c_a; &c_m;&c_je;&c_ch;&c_k;&c_a;</trd>
   <trd lng="ca">&oacute;s rentador</trd>
   <trd lng="de">Waschb&auml;r</trd>
   <trd lng="en">raccoon</trd>
   <trd lng="es">mapache</trd>
   <trd lng="fr">raton laveur</trd>
   <trdgrp lng="he">
     <trd>&dalet;&bet;&yod;&bet;&vav;&fnun;</trd>,
     <trd>&resh;&qof;&vav;&fnun;</trd>
   </trdgrp>
   <trd lng="hu">mos&oacute;medve</trd>
   <trdgrp lng="it">
   <trd>orsetto<klr> lavatore</klr></trd>, 
   <trd>procione</trd>
   </trdgrp>
   <trd lng="ja">&#x30A2;&#x30E9;&#x30A4;&#x30B0;&#x30DE;</trd>
  <trd lng="nl">wasbeer</trd>
   <trd lng="pl">szop pracz</trd>
  <trdgrp lng="pt">
    <trd>racum</trd>,  
    <trd>m&atilde;o-pelada</trd>,
  </trdgrp>
  <trd lng="ro">raton</trd>
   <trd lng="ru">&c_je;&c_n;&c_o;&c_t;</trd>
   <trd lng="uk">&c_jeu;&c_n;&c_o;&c_t;</trd>
</drv>

<drv mrk="urs.GrandaU0ino">
  <kap>Granda <tld lit="U"/>ino
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt></kap><mlg>UMa</mlg>
  <snc mrk="urs.GrandaU0ino.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Granda &ccirc;erpilforma <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref>
      klare videbla en la norda
      hemisfero
      (<trd lng="la">Ursa Major</trd>):
      <ekz>
        malproksime, vualita de nebulo, briletis Nilo, supre brilis la
        sep steloj de la Granda <tld lit="U"/>ino
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 20a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_V;&c_ja;&c_l;&c_ib;&c_k;&c_a;&c_ja; &c_M;&c_ja;&c_d;&c_z;&c_mol;&c_v;&c_je;&c_d;&c_z;&c_ib;&c_h;&c_a; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#x2019;&c_je;)</klr></trd>,
    <trd>&c_V;&c_ja;&c_l;&c_ib;&c_k;&c_ib; &c_V;&c_o;&c_z; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#x2019;&c_je;)</klr></trd>,
    <trd>&c_Ib;&c_l;&c_mol;&c_l;&c_jo;&c_w; &c_V;&c_o;&c_z; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#x2019;&c_je;)</klr></trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>Kastell-karr-bras</trd>,
    <trd>Kastell-karr-kamm</trd>,
    <trd>Karr-Arzhur</trd>
  </trdgrp>
  <trd lng="ca">Ossa Major</trd>
  <trdgrp lng="cs">
    <trd>Velk&aacute; Medv&ecaron;dice</trd>,
    <trd>Velk&yacute; Medv&ecaron;d</trd>,
    <trd>Velk&yacute; V&uring;s</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gro&szlig;er B&auml;r</trd>,
    <trd>Gro&szlig;er Wagen</trd>
  </trdgrp>
  <trd lng="el">&My;&epsilon;&gamma;&alfa_ton;&lambda;&eta; &Alfa_ton;&rho;&kappa;&tau;&omikron;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>Greater Bear</trd>,
    <trd>Great Bear</trd>
  </trdgrp>
  <trd lng="es">Osa Mayor</trd>
  <trdgrp lng="fi">
    <trd>Otava</trd>,
    <trd>Iso Karhu</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Grand charriot</trd>,
    <trd>Grande Ourse</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&ayin;&gimel;&lamed;&he; &gimel;&dalet;&vav;&lamed;&he; Agala gdola</trd>,
    <trd>&dalet;&vav;&bet;&he; &gimel;&dalet;&vav;&lamed;&he; Duba gdola</trd>
  </trdgrp>
  <trd lng="hu">Nagy Medve</trd>
  <trdgrp lng="it">
    <trd>Orsa Maggiore</trd>,
    <trd>Grande Carro</trd>
  </trdgrp>
  <trd lng="nl">Grote <ind>Beer</ind></trd>
  <trd lng="pl">Wielka Nied&zacute;wiedzica  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="pt">Ursa Maior</trd>
  <trd lng="ro">Ursa Mare  <klr>(constela&#x21B;ie)</klr></trd>
  <trd lng="ru">&c_B;&c_o;&c_l;&c_mol;&c_sh;&c_a;&c_ja; &c_M;&c_je;&c_d;&c_v;&c_je;&c_d;&c_i;&c_c;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>Ve&lcaron;k&yacute; voz</trd>,
    <trd>Ve&lcaron;k&aacute; medvedica</trd>
  </trdgrp>
  <trdgrp lng="sl">
    <trd>Veliki medved</trd>,
    <trd>Veliki voz</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>B&uuml;y&uuml;k Ay&inodot;</trd>,
    <trd>B&uuml;y&uuml;kay&inodot;</trd>
  </trdgrp>
</drv>

<drv mrk="urs.MalgrandaU0ino">
  <kap>Malgranda <tld lit="U"/>ino
