<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: urs.xml,v 1.50 2021/05/28 17:06:29 revo Exp $">
<kap>
   <ofc>*</ofc> <rad>urs</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="urs.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="urs.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Peza <ref tip="lst" cel="best.rabo0o" lst="voko:rabobestoj">rabobesto</ref> kun dika hararo, 
      mallonga kolo, tre mallonga vosto,
      kvinfingra, plandmar&scirc;anta, <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio <ref tip="malprt" cel="ursed.0oj">ursedoj</ref> (<trd lng="la">Ursus</trd>):
      <ekz>
        murmuregas la <tld/>o sed danci &gcirc;i devas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        fari al iu <tld/>an <klr>(mallertan)</klr> kareson
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>anta&ubreve; mortigo de <tld/>o ne vendu &gcirc;ian felon</ind>
        <trd lng="br">arabat <ind>gwerzha&ntilde; ar vi e revr ar
        yar</ind></trd>
        <trd lng="fr">il ne faut pas <ind>vendre la peau de l'ours
        avant
        de l'avoir tu&eacute;</ind></trd>
        <trd lng="nl">Je moet het <ind>vel</ind> van de beer niet
        verkopen, voor hij geschoten is.</trd>
        <klr>(atendu la venkon anta&ubreve; ol fanfaroni)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        fari al iu <tld/>an helpon <klr>(dan&gcirc;eran pro mallerto,
        kiel la <tld/>o, kiu la&ubreve; fablo por elpeli mu&scirc;on de dormanto
        frakasas per &scirc;tono lian kapon)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ja;&c_d;&c_z;&c_mol;&c_v;&c_je;&c_d;&c_z;&c_mol;</trd>
  <trd lng="bg">&c_m;&c_je;&c_ch;&c_k;&c_a;</trd>
  <trd lng="bo">&#xF51;&#xF7C;&#xF58;&#xF0B;</trd>
  <trd lng="br">arzh</trd>
  <trdgrp lng="cs">
    <trd>medv&ecaron;d</trd>,
    <trd>medv&ecaron;d</trd>
  </trdgrp>
  <trd lng="de">B&auml;r</trd>
  <trd lng="el">&alfa;&rho;&kappa;&omikron;&ypsilon_ton;&delta;&alfa;</trd>
  <trd lng="en">bear</trd>
  <trd lng="es">oso</trd>
  <trd lng="fi">karhu</trd>
  <trd lng="fr">ours</trd>
  <trd lng="he">&dalet;&bet;</trd>
  <trd lng="hu">medve</trd>
  <trd lng="id">beruang</trd>
  <trd lng="it">orso</trd>
  <trd lng="ne">&#x92B;&#x932;&#x93E;&#x909;&#x928;</trd>
  <trd lng="nl">beer</trd>
  <trd lng="pl">nied&zacute;wied&zacute;</trd>
  <trd lng="pt">urso</trd>
  <trd lng="ru">&c_m;&c_je;&c_d;&c_v;&c_je;&c_d;&c_mol;</trd>
  <trd lng="sk">medve&dcaron;</trd>
  <trd lng="sv">bj&ouml;rn</trd>
  <trd lng="tr">ay&inodot;</trd>
</drv>

<drv mrk="urs.blanka0o">
  <kap>blanka <tld/>o</kap>
  <snc mrk="urs.blanka0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="urs.0o" lst="voko:rabobestoj"><tld lit="U"/>o</ref>
       kun blanka &gcirc;is kremkolora hararo;
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:zoologiaj_specioj">specio</ref> vivanta en Arkto
      (<trd lng="la">Ursus maritimus</trd>):
      <ekz>
        67-jara viro rakontis, ke li travivis atakon de
        blanka <tld/>o en Nunavuto, Kanado
        <fnt>
          <aut>Arono kaj Karlina</aut>,
          <vrk><url ref="http://radioverda.com/">
          Radio Verda</url></vrk>,
          <lok>rv171, 2010-09-04</lok>
        </fnt>.
      </ekz>
    </dif>    
  </snc>
  <trd lng="bg">&c_b;&c_ja;&c_l;&c_a; &c_m;&c_je;&c_ch;&c_k;&c_a;</trd>
  <trd lng="cs">pol&aacute;rn&iacute; medv&ecaron;d</trd>
  <trd lng="de">Eisb&auml;r</trd>
  <trd lng="en">polar bear</trd>
  <trdgrp lng="fr">
    <trd>ours blanc</trd>,
    <trd>ours polaire</trd>
  </trdgrp>
  <trd lng="id"><ind>beruang</ind> kutub</trd>
  <trd lng="it">orso<klr> polare</klr></trd>
  <trd lng="nl">ijsbeer</trd>
  <trdgrp lng="pl">
    <trd>nied&zacute;wied&zacute; polarny</trd>,
    <trd>bia&lstroke;y <ind>nied&zacute;wied&zacute;</ind></trd>
  </trdgrp>
  <trd lng="sk">pol&aacute;rny medve&dcaron;</trd>
</drv>

<drv mrk="urs.lav0o">
  <kap>lav<tld/>o, <var><kap>lav<tld/>eto</kap></var></kap>
  <snc mrk="urs.lav0o.ZOO">
     <uzo tip="fak">ZOO</uzo>
     <dif>
       Hunde granda <ref tip="lst" cel="best.rabo0o" lst="voko:rabobestoj">rabobesto</ref> el Nord- kaj Centrameriko,
       kun griza felo, nigra-blankaj strioj sur la kapo kaj la vosto, 
       enakviganta nutra&jcirc;ojn, anta&ubreve; ol &gcirc;i man&gcirc;as ilin;
       <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref>
       el la familio 
       <ref tip="malprt" cel="procioned.0oj">procionedoj</ref>
       (<trd lng="la">Procyon</trd>):
       <ekz>
         sur la strato mi vidis lav<tld/>on
         <fnt>
           <aut>vik1887</aut>,
           <vrk><url ref="http://vik1887.livejournal.com/">
           Vik en Esperantolando</url></vrk>,
           <lok> 2005-12-28</lok>
         </fnt>;
       </ekz>
       <ekz>
         lav<tld/>eto <klr>...</klr> anka&ubreve; nomi&gcirc;as prociono
         <fnt>
           <aut>T. Alexander</aut>,
           <vrk> Besto- kaj planto-nomoj</vrk>,
           <lok><url ref="http://groups.google.com/group/bablo/">
           bablo</url>, 2007-10-05</lok>
         </fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_ja;&c_n;&c_o;&c_t;</trd>
   <trd lng="bg">&c_m;&c_i;&c_je;&c_shch;&c_a; &c_m;&c_je;&c_ch;&c_k;&c_a;</trd>
   <trd lng="ca">&oacute;s rentador</trd>
   <trd lng="de">Waschb&auml;r</trd>
   <trd lng="en">raccoon</trd>
   <trd lng="es">mapache</trd>
   <trd lng="fr">raton laveur</trd>
   <trdgrp lng="he">
     <trd>&dalet;&bet;&yod;&bet;&vav;&fnun;</trd>,
     <trd>&resh;&qof;&vav;&fnun;</trd>
   </trdgrp>
   <trd lng="hu">mos&oacute;medve</trd>
   <trdgrp lng="it">
   <trd>orsetto<klr> lavatore</klr></trd>, 
   <trd>procione</trd>
   </trdgrp>
   <trd lng="nl">wasbeer</trd>
   <trd lng="pl">szop pracz</trd>
  <trdgrp lng="pt">
    <trd>racum</trd>,  
    <trd>m&atilde;o-pelada</trd>,
  </trdgrp>
   <trd lng="ru">&c_je;&c_n;&c_o;&c_t;</trd>
   <trd lng="uk">&c_jeu;&c_n;&c_o;&c_t;</trd>
</drv>

<drv mrk="urs.GrandaU0ino">
  <kap>Granda <tld lit="U"/>ino
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt></kap><mlg>UMa</mlg>
  <snc mrk="urs.GrandaU0ino.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Granda &ccirc;erpilforma <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref> klare videbla en la norda
      hemisfero
      (<trd lng="la">Ursa Major</trd>):
      <ekz>
        malproksime, vualita de nebulo, briletis Nilo, supre brilis la
        sep steloj de la Granda <tld lit="U"/>ino
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 20a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="br">
    <trd>Kastell-karr-bras</trd>,
    <trd>Kastell-karr-kamm</trd>,
    <trd>Karr-Arzhur</trd>
  </trdgrp>
  <trd lng="ca">Ossa Major</trd>
  <trdgrp lng="cs">
    <trd>Velk&aacute; Medv&ecaron;dice</trd>,
    <trd>Velk&yacute; Medv&ecaron;d</trd>,
    <trd>Velk&yacute; V&uring;s</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gro&szlig;er B&auml;r</trd>,
    <trd>Gro&szlig;er Wagen</trd>
  </trdgrp>
  <trd lng="el">&My;&epsilon;&gamma;&alfa_ton;&lambda;&eta; &Alfa_ton;&rho;&kappa;&tau;&omikron;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>Greater Bear</trd>,
    <trd>Great Bear</trd>
  </trdgrp>
  <trd lng="es">Osa Mayor</trd>
  <trdgrp lng="fi">
    <trd>Otava</trd>,
    <trd>Iso Karhu</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Grand charriot</trd>,
    <trd>Grande Ourse</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&ayin;&gimel;&lamed;&he; &gimel;&dalet;&vav;&lamed;&he; Agala gdola</trd>,
    <trd>&dalet;&vav;&bet;&he; &gimel;&dalet;&vav;&lamed;&he; Duba gdola</trd>
  </trdgrp>
  <trd lng="hu">Nagy Medve</trd>
  <trdgrp lng="it">
    <trd>Orsa Maggiore</trd>,
    <trd>Grande Carro</trd>
  </trdgrp>
  <trd lng="nl">Grote <ind>Beer</ind></trd>
  <trd lng="pl">Wielka Nied&zacute;wiedzica</trd>
  <trd lng="pt">Ursa Maior</trd>
  <trd lng="ru">&c_B;&c_o;&c_l;&c_o;&c_mol;&c_sh;&c_a;&c_ja; &c_M;&c_je;&c_d;&c_v;&c_je;&c_d;&c_i;&c_c;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>Ve&lcaron;k&yacute; voz</trd>,
    <trd>Ve&lcaron;k&aacute; medvedica</trd>
  </trdgrp>
  <trdgrp lng="sl">
    <trd>Veliki medved</trd>,
    <trd>Veliki voz</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>B&uuml;y&uuml;k Ay&inodot;</trd>,
    <trd>B&uuml;y&uuml;kay&inodot;</trd>
  </trdgrp>
</drv>

<drv mrk="urs.MalgrandaU0ino">
  <kap>Malgranda <tld lit="U"/>ino
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt></kap><mlg>UMi</mlg>
  <snc mrk="urs.MalgrandaU0ino.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      La norda <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref>
      &ccirc;erpilforma, en kiu la ekstrema
      stelo de la &ccirc;erpiltenilo estas la <ref tip="prt" cel="stel.polusa0o.AST">Polusa Stelo</ref>
      (<trd lng="la">Ursa Minor</trd>).
    </dif>
    <trd lng="br">Kastell-karr-kamm bihan</trd>
    <trdgrp lng="cs">
      <trd>Mal&yacute; Medv&ecaron;d</trd>,
      <trd>Mal&yacute; V&uring;z</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Kleiner B&auml;r<klr tip="amb"> (Sternbild)</klr></trd>,
      <trd>Kleiner Wagen<klr tip="amb"> (Sternbild)</klr></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>Little Bear</trd>,
      <trd>Little Dipper</trd>,
      <trd>Ursa Minor</trd>
    </trdgrp>
    <trd lng="es">Osa Menor</trd>
    <trdgrp lng="fi">
      <trd>Pieni Karhu</trd>,
      <trd>Pikku Otava</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>Petit chariot</trd>,
      <trd>Petite Ourse</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>Kismedve</trd>,
      <trd>Kisg&ouml;nc&ouml;l</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>Orsa Minore</trd>,
      <trd>Piccolo Carro</trd>
    </trdgrp>
    <trdgrp lng="he">
      <trd>&he;&dalet;&vav;&bet;&he; &he;&qof;&tet;&nun;&he;</trd>,
      <trd>&he;&ayin;&gimel;&lamed;&he; &he;&qof;&tet;&nun;&he;</trd>
    </trdgrp>
    <trd lng="nl">Kleine <ind>Beer</ind></trd>
    <trd lng="pl">Ma&lstroke;a Nied&zacute;wiedzica</trd>
    <trd lng="pt">Ursa Menor</trd>
    <trd lng="ru">&c_M;&c_a;&c_l;&c_a;&c_ja; &c_M;&c_je;&c_d;&c_v;&c_je;&c_d;&c_i;&c_c;&c_a;</trd>
  </snc>
</drv>
</art>
