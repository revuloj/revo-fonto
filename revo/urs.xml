<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$"> <kap>
<ofc>*</ofc> <rad>urs</rad>/o </kap> <drv mrk="urs.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="ursed.0oj"><tld/>edoj</ref>
      (<trd lng="la">Ursus</trd>),
      plumpa rabobesto kun mallonga dika kolo,
      kvinfingra, plandmar&scirc;anta, kun tre mallonga vosto:
      <ekz>
        murmuregas la <tld/>o sed  danci &gcirc;i devas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        fari al iu <tld/>an (mallertan) kareson
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        anta&ubreve; mortigo de <tld/>o ne vendu &gcirc;ian felon
        <klr>(atendu la venkon anta&ubreve; ol fanfaroni:
        <refgrp tip="vid">
          <ref cel="fru">tro frue</ref>,
          <ref cel="triumf.0i">triumfi</ref>
        </refgrp>
        )</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        fari al iu <tld/>an helpon <klr>(dan&gcirc;eran pro mallerto,
        kiel la <tld/>o, kiu por elpeli mu&scirc;on de sia dormanta
amiko,
        frakasis
        per &scirc;tono lian kapon kune kun la mu&scirc;o)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd
lng="be">&c_m;&c_ja;&c_d;&c_z;&c_mol;&c_v;&c_je;&c_d;&c_z;&c_mol;</trd>
  <trd lng="cs">medv&ecaron;d</trd>
  <trd lng="de">B&auml;r</trd>
  <trd
lng="el">&alfa;&rho;&kappa;&omikron;&ypsilon_ton;&delta;&alfa;</trd>
  <trd lng="en">bear</trd>
  <trd lng="es">oso</trd>
  <trd lng="fr">ours</trd>
  <trd lng="he">&dalet;&bet;</trd>
  <trd lng="hu">medve</trd>
  <trd lng="it">orso</trd>
  <trd lng="nl">beer</trd>
  <trd lng="pt">urso</trd>
  <trd lng="ru">&c_m;&c_je;&c_d;&c_v;&c_je;&c_d;&c_mol;</trd>
  <trd lng="sv">bj&ouml;rn</trd>
  <trd lng="tr">ay&inodot;</trd>
</drv>

<drv mrk="urs.MalgrandaU0ino">
  <kap>Malgranda <tld lit="U"/>ino</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      La norda <ref tip="super" cel="stel.0aro">stelaro</ref>
      &ccirc;erpilforma (<trd lng="la">Ursa Minor</trd>); la ekstrema
      stelo de la &ccirc;erplitenilo estas la
      <ref tip="prt" cel="polus.P0astelo">Polusa stelo</ref>.
    </dif>
    <trdgrp lng="cs">
      <trd>Mal&yacute; Medv&ecaron;d</trd>,
      <trd>Mal&yacute; V&#x16F;z</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>Little Bear</trd>,
      <trd>Little Dipper</trd>,
      <trd>Ursa Minor</trd>
    </trdgrp>
    <trd lng="es">Osa Menor</trd>
    <trdgrp lng="hu">
      <trd>Kismedve</trd>,
      <trd>Kisg&ouml;nc&ouml;l</trd>
    </trdgrp>
  <trdgrp lng="he">
    <trd>&he;&dalet;&vav;&bet;&he; &he;&qof;&tet;&nun;&he;</trd>,
    <trd>&he;&ayin;&gimel;&lamed;&he; &he;&qof;&tet;&nun;&he;</trd>
  </trdgrp>
    <trd lng="pt">Ursa Menor</trd>
    <trd lng="ru">&c_M;&c_a;&c_l;&c_a;&c_ja;
    &c_M;&c_je;&c_d;&c_v;&c_je;&c_d;&c_i;&c_c;&c_a;</trd>
  </snc>
</drv>
</art>
