<kap>
   <rad>tantal</rad>/o
   <fnt>Z</fnt>
</kap>
<drv mrk="tantal.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Ta</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
       &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="73">elemento</ref> kun
       atomnumero 73, atompezo 180,948; malofta ar&gcirc;entblanka 
       transir<ref tip="super" cel="metal.0o">metalo</ref>:
      <ekz>
        <tld/>o estas uzata en kirurgiaj iloj kaj enplanta&jcirc;oj, &ccirc;ar &gcirc;i ne reakcias kun korpaj fluidoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=24511">Tantalo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    <ref tip="hom" cel="tantal1.0o">Tantalo</ref>
    anka&ubreve; nomi&gcirc;as mitologia prapatro.
  </rim>
  <trd lng="be">&c_t;&c_a;&c_n;&c_t;&c_a;&c_l;</trd>
  <trd lng="bg">&c_t;&c_a;&c_n;&c_t;&c_a;&c_l;</trd>
  <trd lng="br">tantal</trd>
  <trd lng="ca">t&agrave;ntal</trd>
  <trdgrp lng="cs">
    <trd>Ta</trd>,
    <trd>tantal</trd>
  </trdgrp>
  <trd lng="de">Tantal</trd>
  <trd lng="en">tantalum</trd>
  <trd lng="es">t&aacute;ntalo</trd>
  <trd lng="fr">tantale</trd>
  <trd lng="hu">tant&aacute;l</trd>
  <trd lng="id">tantalum</trd>
  <trd lng="it">tantalio</trd>
  <trd lng="ja">&#x30BF;&#x30F3;&#x30BF;&#x30EB;</trd>
  <trd lng="la">Tantalum</trd>
  <trd lng="nl">tantaal</trd>
  <trd lng="pl">tantal</trd>
  <trd lng="pt">t&acirc;ntalo</trd>
  <trd lng="ru">&c_t;&c_a;&c_n;&c_t;&c_a;&c_l; <klr tip="amb">(&c_m;&c_je;&c_t;&c_a;&c_l;&c_l;)</klr></trd>
  <trdgrp lng="sk">
    <trd>Ta</trd>,
    <trd>tantal</trd>
  </trdgrp>
  <trd lng="sv">tantal</trd>
  <trd lng="uk">&#x442;&#x430;&#x43D;&#x442;&#x430;&#x43B;</trd>
