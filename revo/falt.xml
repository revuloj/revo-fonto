<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: falt.xml,v 1.23 2023/08/31 13:11:27 revo Exp $">
<kap>
<rad>falt</rad>/o<fnt><bib>KrED</bib></fnt>
</kap>
<drv mrk="falt.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">ANA</uzo>
    <dif>
      Ha&ubreve;t<ref tip="super" cel="sulk.0o.KOMUNE">sulk</ref>eto:
      <ekz>
        li demetis al si la okulvitrojn kaj masa&gcirc;is al si la du ru&gcirc;etajn <tld/>ojn
        amba&ubreve;flanke de la nazo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Monato;/2003/007991.html">Espero</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la fingroj <tld/>i&gcirc;as (alivorte sulki&gcirc;as, kuntiri&gcirc;as)
        post longa restado en akvo
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011217.php">Nekovrilaj malkovroj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_m;&c_a;&c_r;&c_sh;&c_ch;&c_y;&c_n;&c_a;</trd>,
      <trd>&c_z;&c_m;&c_o;&c_r;&c_sh;&c_ch;&c_y;&c_n;&c_a;</trd>,
      <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_k;&c_a; <klr tip="amb">(&c_s;&c_k;&c_u;&c_r;&c_y;)</klr></trd>
    </trdgrp>
    <trd lng="ca">arruga</trd>
    <trd lng="es">arruga</trd>
    <trdgrp lng="en">
      <trd>wrinkle</trd>,
      <trd>crease</trd>
    </trdgrp>
    <trd lng="fr">ride<klr tip="ind"> (cutan&eacute;e)</klr></trd>
    <trd lng="hu">r&aacute;nc</trd>
    <trd lng="it">ruga</trd>
    <trd lng="nl">rimpel</trd>
    <trd lng="pl">zmarszczka</trd>
    <trd lng="pt">ruga</trd>
    <trdgrp lng="ru">
      <trd>&c_m;&c_o;&c_r;&c_shch;&c_i;&c_n;&c_a;</trd>,
      <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_k;&c_a; <klr>(&c_k;&c_o;&c_zh;&c_i;)</klr></trd>
    </trdgrp>
  </snc>
  <snc>
  <uzo tip="fak">GEOL</uzo>
  <dif>
    Sulkego en la tero.
  </dif>
  <trd lng="be">&c_s;&c_k;&c_l;&c_a;&c_d;&c_k;&c_a;&c_v;&c_a;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="es">pliegue</trd>
  <trdgrp lng="en">
    <trd>plication</trd>,
    <trd>fold <klr tip="amb">(geol.)</klr></trd>
  </trdgrp>
  </snc>
  <trd lng="cs">vr&aacute;sa</trd>
  <trd lng="de">Falte</trd>
  <trd lng="ja">&#x3057;&#x308F;</trd>
  <trdgrp lng="sk">
    <trd>br&aacute;zda</trd>,
    <trd>vr&aacute;ska</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43A;&#x43B;&c_a;&c_d;&#x43A;&c_a; (&c_sh;&#x43A;&c_ib;&c_r;&c_i;)</trd>,
    <trd>&c_z;&#x43C;&#x43E;&c_r;&c_sh;&#x43A;&c_a;</trd>,
    <trd>&c_s;&#x43A;&#x43B;&c_a;&c_d;&#x43A;&c_a;</trd>,
    <trd>&c_s;&#x43A;&#x43B;&c_a;&c_d;&c_ch;&c_a;&c_s;&c_t;&c_ib;&c_s;&c_t;&#x44C;</trd>
  </trdgrp>
</drv>
</art>
