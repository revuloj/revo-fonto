<vortaro>
<art mrk="$Id$">
<kap>
<rad>falt</rad>/o<fnt><bib>KrED</bib></fnt>
</kap>
<drv mrk="falt.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">ANA</uzo>
    <dif>
      Ha&ubreve;tsulketo  
    </dif>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_r;&c_sh;&c_ch;&c_y;&c_n;&c_a;</trd>,
    <trd>&c_z;&c_m;&c_o;&c_r;&c_sh;&c_ch;&c_y;&c_n;&c_a;</trd>,
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_k;&c_a; <klr tip="amb">(&c_s;&c_k;&c_u;&c_r;&c_y;)</klr></trd>
  </trdgrp>
  <trd lng="ca">arruga</trd>
  <trd lng="es">arruga</trd>
  <trdgrp lng="en">
    <trd>wrinkle</trd>,
    <trd>crease</trd>
  </trdgrp>
  <trd lng="fr">ride<klr tip="ind"> (cutan&eacute;e)</klr></trd>
  <trd lng="hu">r&aacute;nc</trd>
  <trd lng="it">ruga</trd>
  <trd lng="nl">rimpel</trd>
  <trd lng="pl">zmarszczka</trd>
  <trd lng="pt">ruga</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_r;&c_shch;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_k;&c_a; <klr>(&c_k;&c_o;&c_zh;&c_i;)</klr></trd>
  </trdgrp>
  </snc>
  <snc>
  <uzo tip="fak">GEOL</uzo>
  <dif>
    Sulkego en la tero
  </dif>
  <trd lng="es">pliegue</trd>
  <trdgrp lng="en">
    <trd>plication</trd>,
    <trd>fold <klr tip="amb">(geol.)</klr></trd>
  </trdgrp>
  </snc>
  <trd lng="cs">vrása</trd>
  <trd lng="de">Falte</trd>
  <trdgrp lng="sk">
    <trd>brázda</trd>,
    <trd>vráska</trd>
  </trdgrp>
</drv>
</art>
