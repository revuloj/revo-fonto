<drv mrk="epiglo.0o">
<kap><tld/>o</kap>
<uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      <ref cel="laring.0o">Laringa</ref>
      valvo fermanta la eniron de la spira kanalo, kiam oni ion
      englutas:
      <ekz>
        laringo <klr>[&#x2026;]</klr> estas fermebla al <ref cel="faring.0o">faringo</ref>
        per <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=35229">Laringo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a konsonanto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=7574">Konsonanto</url></lok>
        </fnt>.
      </ekz> 
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_d;&c_g;&c_o;&c_r;&c_l;&c_ib;&c_ch;&c_n;&c_y; &c_h;&c_r;&c_a;&c_s;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_g;&c_a;&c_r;&c_t;&c_a;&c_n;&c_n;&c_y; &c_h;&c_r;&c_a;&c_s;&c_t;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">epiglotis</trd>
  <trdgrp lng="cs">
    <trd>epiglottis</trd>,
    <trd>p&rcaron;&iacute;klopka hrtanov&aacute;</trd>
  </trdgrp>
  <trd lng="de">Kehldeckel</trd>
  <trd lng="en">epiglottis</trd>
  <trd lng="es">epiglotis</trd>
  <trd lng="fr">&eacute;piglotte</trd>
  <trd lng="it">epiglottide</trd>
  <trd lng="ja">&#x5589;&#x982D;&#x84CB;  <pr>&#x3053;&#x3046;&#x3068;&#x3046;&#x304C;&#x3044;</pr></trd>
  <trd lng="pl">nag&lstroke;o&sacute;nia</trd>
  <trd lng="pt">epiglote</trd>
  <trd lng="ru">&c_n;&c_a;&c_d;&c_g;&c_o;&c_r;&c_t;&c_a;&c_n;&c_n;&c_y;&c_j; &c_h;&c_r;&c_ja;&c_shch;</trd>
  <trd lng="sk">hrtanov&aacute; pr&iacute;klopka</trd>
