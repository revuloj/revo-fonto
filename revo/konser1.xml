<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>3</ofc>
      <rad>konservativ</rad>/a
      <fnt>Z</fnt>
    </kap>

    <drv mrk="konser1.0a">
      <kap><tld/>a</kap>
      <uzo tip="fak">POL</uzo>
      <snc>
        <dif>
          Politike kaj sociale
          <ref tip="sin" cel="konser.0ema">konservema</ref>;
          kontra&ubreve;a al &scirc;an&gcirc;oj kaj reformoj:
          <ekz>
            la Nacia Partio, tradicie <tld/>a, teorie favora al libera
            merkato, sed fakte intervenema
            <fnt><aut>Donald Rogers</aut>:
              <vrk>Ekonomia miraklo a&ubreve; malsano?</vrk>,
              <bib>Monato</bib>
              <lok>jaro 1997a, numero 2a, p. 14a</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <ref tip="sin" cel="konser.0isma">konservisma</ref>
      <trd lng="be">&c_k;&c_a;&c_n;&c_s;&c_e;&c_r;&c_v;&c_a;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>
      <trdgrp lng="de">
        <trd>konservativ</trd>,
        <trd>altmodisch</trd>
      </trdgrp>
      <trd lng="en">conservative</trd>
      <trd lng="es">conservador</trd>
      <trdgrp lng="fr">
        <trd>conservateur<klr> (adj., politique)</klr></trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>conservatief<klr tip="ind"> (bn.)</klr></trd>,
        <trd>behoudend</trd>
      </trdgrp>
      <trd lng="pt">conservador<klr tip="amb"> (adj.)</klr></trd>
      <trd lng="ru">&c_k;&c_o;&c_n;&c_s;&c_je;&c_r;&c_v;&c_a;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
    </drv>

    <drv mrk="konser1.0ismo">
      <kap><tld/>ismo</kap>
      <uzo tip="fak">POL</uzo>
      <snc>
        <dif>
          Socia kaj politika doktrino, kiu celas precipe konservadon de
          la
          nune funkcianta socio:
          <ekz>
            establi&gcirc;is maldekstraj registaroj post periodo de
            <tld/>ismo
            <fnt><aut>Stefan Maul</aut>:
              <vrk>Furoras popolistoj</vrk>,
              <bib>Monato</bib><lok>jaro 2002a, numero 6a, p. 8a</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <ref tip="sin" cel="konser.0ismo">konservismo</ref>
      <trd lng="be">&c_k;&c_a;&c_n;&c_s;&c_e;&c_r;&c_v;&c_a;&c_t;&c_y;&c_z;&c_m;</trd>
      <trd lng="en">conservatism</trd>
      <trd lng="es">conservadurismo</trd>
      <trdgrp lng="fr">
        <trd>conservatisme<klr> (politique)</klr></trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>conservatisme</trd>,
        <trd>behoudzucht</trd>
      </trdgrp>
      <trd lng="ru">&c_k;&c_o;&c_n;&c_s;&c_je;&c_r;&c_v;&c_a;&c_t;&c_i;&c_z;&c_m;</trd>
    </drv>

    <drv mrk="konser1.0ulo">
      <kap><tld/>ulo</kap>
      <uzo tip="fak">POL</uzo>
      <snc>
        <dif>
          Subtenanto de konserva politiko, de socia ne&scirc;an&gcirc;emo:
          <ekz>
            koaliciano por la <tld/>uloj
            <fnt><aut>Baard Hekland</aut>:
              <vrk>Triumfas tedulo</vrk>, <bib>Monato</bib>
              <lok>jaro 2001a, numero 11a, p. 6a</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_k;&c_a;&c_n;&c_s;&c_e;&c_r;&c_v;&c_a;&c_t;&c_a;&c_r;</trd>
      <trd lng="en">conservative</trd>
      <trd lng="es">conservador</trd>
      <trdgrp lng="fr">
        <trd>conservateur<klr> (subst., politique)</klr></trd>
      </trdgrp>
      <trd lng="nl">conservatief<klr tip="ind"> (zn.)</klr></trd>
      <trd lng="pt">conservador<klr tip="amb"> (subst.)</klr></trd>
      <trd lng="ru">&c_k;&c_o;&c_n;&c_s;&c_je;&c_r;&c_v;&c_a;&c_t;&c_o;&c_r;</trd>
    </drv>
  </art>
