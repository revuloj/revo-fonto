<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>meandr</rad>/o
</kap>
<drv mrk="meandr.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Duon- &gcirc;is preska&ubreve; plen-cirkla kurbi&gcirc;o de rivero.
    </dif>
  </snc>
  <snc>
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Ornamfiguro el rektangule rompitaj neinterrompitaj linioj,
      ofte renkontata en la antikva arto de la grekoj.
    </dif>
  </snc>
  <trd lng="nl">meander</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_v;&c_i;&c_l;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_i;&c_z;&c_g;&c_i;&c_b;</trd>,
    <trd>&c_m;&c_je;&c_a;&c_n;&c_d;&c_r;</trd>
  </trdgrp>
</drv>
</art>
