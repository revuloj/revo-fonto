<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mamon.xml,v 1.15 2025/12/26 16:39:28 revo Exp $">
<kap>
  <rad>mamon</rad>/o
  <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="mamon.0o">
  <kap>
    <tld/>o
  </kap>
    <snc>
    <uzo tip="fak">BIB</uzo>
    <dif>
      Personigo de la materiala ri&ccirc;eco:
      <ekz>
        <tld/>o estas ilia dio.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_m;&c_o;&c_n;&c_a;</trd>
  <trd lng="ca">Mammon</trd>
  <trd lng="de">Mammon</trd>
  <trd lng="en">mammon</trd>
  <trd lng="fr">Mammon</trd>
  <trd lng="hu">mammon</trd>
  <trdgrp lng="id">
    <trd>mamon</trd>,
    <trd>uang</trd>,
    <trd><baz>kaya</baz>kekayaan</trd>
  </trdgrp>
  <trd lng="nl">Mammon</trd>
  <trd lng="pl">mamona</trd>
  <trd lng="pt">Mammon</trd>
  <trd lng="ru">&c_m;&c_a;&c_m;&c_o;&c_n;&c_a;</trd>
</drv>
</art>
