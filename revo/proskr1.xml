<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: proskr1.xml,v 1.12 2018/10/24 19:10:18 revo Exp $">
<kap>
  <rad>proskripci</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="proskr1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="proskr.0o">Proskribo</ref>:
      <ekz>
        <nom>Tostig</nom> estis <tld/>ata kaj senigata de sia graflando, kiun la re&gcirc;o konfidis al <nom>Morcar</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=280151">Tostig Godwinson</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Hereward the Wake</nom> <klr>[&#8230;]</klr> unue ribelis kontra&ubreve; Eduardo la
        Konfesanto en 1054, pro ties uzo de normandaj konsilistoj, kaj estis <tld/>ata
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=280210">Hereward the Wake</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>proskripce</trd>,
    <trd>psanstv&iacute;</trd>,
    <trd>vyho&scaron;t&ecaron;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Proskription</trd>,
    <trd>&Auml;chtung</trd>,
    <trd>Vertreibung</trd>
  </trdgrp>
  <trd lng="fr">proscription</trd>
  <trdgrp lng="hu">
    <trd>sz&aacute;m&udblac;zet&eacute;s<klr tip="ind"> (&iacute;t&eacute;let n&eacute;lk&uuml;li)</klr></trd>,
    <trd>t&ouml;rv&eacute;nyen k&iacute;v&uuml;l helyez&eacute;s</trd>
    <trd>proskripci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>proscriptie</trd>,
    <trd>vogelvrijverklaring</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>proscri&ccedil;&atilde;o</trd>,
    <trd>banimento</trd>,
    <trd>desterro</trd>,
    <trd>degredo</trd>,
    <trd>deporta&ccedil;&atilde;o</trd>    
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_k;&c_r;&c_i;&c_p;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sk">vyhostenie</trd>
</drv>
</art>

