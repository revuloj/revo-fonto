<vortaro>
<art mrk="$Id: proskr1.xml,v 1.15 2022/11/09 09:32:05 revo Exp $">
<kap>
  <rad>proskripci</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="proskr1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="proskr.0o">Proskribo</ref>:
      <ekz>
        <nom>Tostig</nom> estis <tld/>ata kaj senigata de sia graflando, kiun la re&gcirc;o konfidis al
        <nom>Morcar</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=280151">Tostig Godwinson</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Hereward the Wake</nom> <klr>[&#8230;]</klr> unue ribelis kontra&ubreve; Eduardo la
        Konfesanto en 1054, pro ties uzo de normandaj konsilistoj, kaj estis <tld/>ata
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=280210">Hereward the Wake</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_s;&c_k;&c_r;&c_y;&c_p;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_v;&c_y;&c_g;&c_n;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>proskripce</trd>,
    <trd>psanstv&iacute;</trd>,
    <trd>vyho&scaron;t&ecaron;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Proskription</trd>,
    <trd>&Auml;chtung</trd>,
    <trd>Vertreibung</trd>
  </trdgrp>
  <trd lng="fr">proscription</trd>
  <trdgrp lng="hu">
    <trd>sz&aacute;m&udblac;zet&eacute;s<klr tip="ind"> (&iacute;t&eacute;let n&eacute;lk&uuml;li)</klr></trd>,
    <trd>t&ouml;rv&eacute;nyen k&iacute;v&uuml;l helyez&eacute;s</trd>
    <trd>proskripci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>proscriptie</trd>,
    <trd>vogelvrijverklaring</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>proskrypcja</trd>,
    <trd>banicja</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>proscri&ccedil;&atilde;o</trd>,
    <trd>banimento</trd>,
    <trd>desterro</trd>,
    <trd>degredo</trd>,
    <trd>deporta&ccedil;&atilde;o</trd>    
  </trdgrp>
  <trd lng="ro">exilare</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_k;&c_r;&c_i;&c_p;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sk">vyhostenie</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_g;&#x43D;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_v;&c_i;&c_s;&c_i;&#x43B;&#x43A;&c_a;</trd>,
    <trd>&#x43F;&c_r;&#x43E;&c_s;&#x43A;&c_r;&c_i;&#x43F;&c_c;&c_ib;&#x44F;</trd>,
    <trd>&#x43E;&c_g;&#x43E;&#x43B;&#x43E;&c_sh;&c_je;&#x43D;&#x43D;&#x44F; &#x43F;&#x43E;&c_z;&c_a;
    &c_z;&c_a;&#x43A;&#x43E;&#x43D;&#x43E;&#x43C;</trd>
  </trdgrp>
</drv>
</art>

