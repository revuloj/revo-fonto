<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>devi</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="devi.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="devi.0i.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Deloki&gcirc;i de la normala pozicio, direkto:
      <ekz>
        kompasmontrilo <tld/>as sub influo de magneto;
      </ekz>
      <ekz>
        <tld/>inta vertebro.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>kit&eacute;r</trd>,
      <trd>let&eacute;r</trd></trdgrp>

  </snc>
  <snc mrk="devi.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Deklini&gcirc;i de la regula konduto:
      <ekz>
        &scirc;i <tld/>is de la virta vojo<fnt>B</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="turn">deturni&gcirc;i</ref>,
      <ref cel="voj.de0igxiel0iri">devoji&gcirc;i</ref>,
      <ref cel="flank">flankeni&gcirc;i</ref>.
    </refgrp>
    <trd lng="hu">elt&eacute;r<klr tip="ind"> (a norm&aacute;list&oacute;l)</klr></trd>
  </snc>
  <trd lng="de">abweichen</trd>
  <trd lng="es">desviar</trd>
  <trd lng="fr">d&eacute;vier<klr tip="ind"> (intr.)</klr></trd>
  <trd lng="ru">&c_o;&c_t;&c_k;&c_l;&c_o;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="devi.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Nenormala deloki&gcirc;o:
      <ekz>
        stomaka <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Abweichung</trd>
  <trd lng="es">desviaci&oacute;n</trd>
  <trd lng="fr">d&eacute;viation</trd>
  <trdgrp lng="hu">
    <trd>devi&aacute;ci&oacute;</trd>,
    <trd>deviancia</trd>,
    <trd>elhajl&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_k;&c_l;&c_o;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_m;&c_je;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_d;&c_v;&c_i;&c_g;</trd>,
    <trd>&c_i;&c_s;&c_k;&c_r;&c_i;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_u;&c_k;&c_l;&c_o;&c_n;</trd>
  </trdgrp>
</drv>

<drv mrk="devi.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari, ke io <tld/>u:
      <ekz>
        &scirc;toneto <tld/>igis la radon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>d&eacute;vier<klr tip="ind"> (tr.)</klr></trd>,
    <trd>d&eacute;tourner</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>elt&eacute;r&iacute;t</trd>,
    <trd>let&eacute;r&iacute;t</trd>,
    <trd>kit&eacute;r&iacute;t</trd>
  </trdgrp>
</drv>

<drv mrk="devi.0aangulo">
  <kap><tld/>a angulo</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Angulo formata per la direktoj de la radio anta&ubreve; kaj
      post la <tld/>ado.
    </dif>
  </snc>
  <adm>
    Dubinda vorto. Supozeble temas pri malnova formo de "refrakta angulo".
    <aut>MB</aut>
  </adm>
  <trd lng="hu">kit&eacute;r&eacute;si <ind>sz&ouml;g</ind></trd>
  <trd lng="ru">&c_u;&c_g;&c_o;&c_l; &c_o;&c_t;&c_k;&c_l;&c_o;&c_n;&c_je;&c_n;&c_i;&c_ja;</trd>
</drv>

<drv mrk="devi.varianca0o">
  <kap>varianca <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <klr>(de <ref cel="variab.hazarda0o">hazarda variablo</ref> 
      <frm>X</frm>)</klr>
      La kvadrata radiko de &gcirc;ia <ref cel="varian1.0o">varianco</ref>;
      simb. <frm>&sigma;(X)</frm>.
    </dif>
  </snc>
  <trd lng="de">Standardabweichung</trd>
  <trd lng="en">standard <ind>deviation</ind></trd>
  <trd lng="fr">&eacute;cart-type</trd>
  <trdgrp lng="hu">
    <trd>norm&aacute;l <ind>elt&eacute;r&eacute;s</ind></trd>,
    <trd>standard <ind>elt&eacute;r&eacute;s</ind></trd>,
    <trd>sz&oacute;r&aacute;s</trd>
  </trdgrp>
  <trd lng="pl">odchylenie standardowe<klr tip="ind"> (zmiennej losowej)</klr></trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_n;&c_d;&c_a;&c_r;&c_t;&c_n;&c_o;&c_je; <ind>&c_o;&c_t;&c_k;&c_l;&c_o;&c_n;&c_je;&c_n;&c_i;&c_je;</ind></trd>
</drv>
</art>
