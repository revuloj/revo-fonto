<drv mrk="devi.0aangulo">
  <kap><tld/>a angulo</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Angulo formata per la direktoj de la radio anta&ubreve; kaj
      post la <tld/>ado.
    </dif>
  </snc>
  <adm>
    Dubinda vorto. Supozeble temas pri malnova formo de "refrakta
    angulo".
    <aut>MB</aut>
  </adm>
  <adm>
    Se konservi, &scirc;ovi pli bone sub angul', evtl. kiel ekz-o
    <aut>WD</aut>
  </adm>
  <trd lng="be">
  &c_v;&c_u;&c_g;&c_a;&c_l;
  &c_a;&c_d;&c_h;&c_ib;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_ja;
  </trd>
  <trd lng="de">Ablenkungswinkel</trd>
  <trd lng="hu">kit&eacute;r&eacute;si <ind>sz&ouml;g</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x504F;&#x89D2;  <pr>&#12408;&#12435;&#x304B;&#x304F;</pr></trd>,
    <trd>&#12405;&#x308C;&#x306E;&#x89D2;  <pr>&#12405;&#x308C;&#x306E;&#x304B;&#12393;</pr></trd>
  </trdgrp>
  <trd lng="pl">k&aogonek;t za&lstroke;amania</trd>
  <trd lng="ru">
  &c_u;&c_g;&c_o;&c_l;
  &c_o;&c_t;&c_k;&c_l;&c_o;&c_n;&c_je;&c_n;&c_i;&c_ja;
  </trd>
</drv>

<drv mrk="devi.varianca0o">
