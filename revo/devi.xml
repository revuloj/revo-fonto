    <trd>&#x504F;&#x5DEE;  <pr>&#12408;&#12435;&#12373;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zboczenie</trd>,
    <trd>odchy&lstroke;ka</trd>,
    <trd>dewiacja</trd>,
    <trd>odst&eogonek;pstwo</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_k;&c_l;&c_o;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_m;&c_je;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_d;&c_v;&c_i;&c_g;</trd>,
    <trd>&c_i;&c_s;&c_k;&c_r;&c_i;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;
    </trd>,
    <trd>&c_u;&c_k;&c_l;&c_o;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odch&yacute;lka</trd>,
    <trd>&uacute;chylka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_h;&c_i;&c_l;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_k;&c_r;&c_u;&c_ch;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_u;&c_h;&c_i;&c_l;</trd>,
    <trd>&c_v;&c_i;&c_k;&c_r;&c_i;&c_v;&c_l;&c_je;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5DEE;&#x5F02;&#24615; <pr>ch&amacron;y&#xEC;x&#xEC;ng</pr></trd>,
    <trd>&#x8C61;&#x5DEE; <pr>xi&#xE0;ngch&amacron;i</pr></trd>,
    <trd>&#x6B67;&#x5F02; <pr>q&#xED;y&#xEC;</pr></trd>,
    <trd>&#29305;&#x63A1; <pr>t&#xE8;c&#x1CE;i</pr></trd>,
    <trd>&#x4E56;&#x79BB; <pr>gu&amacron;il&#xED;</pr></trd>,
    <trd>&#x8AA4;&#x5DEE; <pr>w&#xF9;ch&amacron;</pr></trd>,
    <trd>&#x504F;&#x5DEE; <pr>pi&amacron;nch&amacron;</pr></trd>,
    <trd>&#x5DEE; <pr>ch&amacron;</pr></trd>,
    <trd>&#x8FDC;&#x8DB3; <pr>yu&#x1CE;nz&#xFA;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="devi.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari, ke io <tld/>u:
      <ekz>
        tiu direkto povas nur <tld/>igi min de la spuro, malhelpi la esploron kaj min
        konfuzi
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        Rafo haste haltigis &scirc;ian parolon, kaj intencis <tld/>igi la babilan tempon
         <fnt>
          <aut>Shi Chengtai</aut>
          <vrk><url ref="&Monato;/1995/A950601.php">La kompensa kiso</url></vrk>
          <bib>Monato</bib>
          <lok>1995/06, p. 27</lok>
        </fnt>;
      </ekz>
      <ekz>
        incitistoj alkuras por <tld/>igi la kornhavulon, sed la toreadoro reekstaras,
        feli&ccirc;e nenio grava
        <fnt>
          <bib>Monato</bib>
          <aut>Eduardo Novembro</aut>
          <vrk><url ref="&Monato;/2014/011318.php">Ta&ubreve;ro??? Festo!!!</url></vrk>
          <lok>2014</lok>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_h;&c_ib;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_r;&c_u;&c_sh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_y;&c_v;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>odklonit</trd>,
    <trd>odklonit</trd>,
    <trd>sto&ccaron;it stranou</trd>,
    <trd>sv&eacute;st</trd>,
    <trd>vych&yacute;lit</trd>
  </trdgrp>
  <trd lng="de">ablenken</trd>
  <trd lng="es">desviar</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;vier<klr tip="ind"> (tr.)</klr></trd>,
    <trd>d&eacute;tourner</trd>
  </trdgrp>
  <trd lng="he">&lamed;&#x5B4;&samekh;&#x5B0;&tet;&vav;&#x5B9;&tav;</trd>
  <trdgrp lng="hu">
    <trd>elt&eacute;r&iacute;t</trd>,
    <trd>let&eacute;r&iacute;t</trd>,
    <trd>kit&eacute;r&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x305D;&#12425;&#12377;</trd>,
    <trd>&#x307E;&#12370;&#x308B;</trd>,
    <trd>&#x304B;&#x305F;&#12424;&#12425;&#x305B;&#x308B;</trd>,
    <trd>&#36920;&#33073;&#12373;&#x305B;&#x308B;  <pr>&#12356;&#12388;&#12384;&#12388;&#12373;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="pl">odchyla&cacute;</trd>
  <trdgrp lng="sk">
    <trd>odkloni&tcaron;</trd>,
    <trd>vych&yacute;li&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_h;&c_i;&c_l;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_b;&c_je;&c_shch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
