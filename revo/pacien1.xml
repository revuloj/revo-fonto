<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>1</ofc> <rad>pacient</rad>/o
    </kap>

    <drv mrk="pacien1.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Iu kuracata de medicinisto a&ubreve; operaciata de &hcirc;irurgiisto:
          <ekz>
            transplantado de organoj fari&gcirc;is ordinara, sava rimedo por
            sennombraj <tld/>oj
            <fnt><aut>Paulo S. Viana</aut>:
              <vrk>&Ccirc;u fordoni siajn organojn?</vrk>,
              <bib>Monato</bib>
              <lok>jaro 2001a, numero 11a, p. 21a</lok></fnt>;
          </ekz><ekz>
            scienca kontrolo de <ind>pacientino</ind>j pruvis, ke la
            progresado de la tumoro kaj la nombro de la mortoj
            malplii&gcirc;is
            <fnt>
              <aut>Evgeni GEORGIEV</aut>:
              <vrk><url ref="http://www.esperanto.be/fel/mon/007233.html"
                  >Novaj kuracmetodoj kaj prevento</url></vrk>.
              <bib>Monato</bib>
            </fnt>.
            <trd lng="ru">&c_p;&c_a;&c_c;&c_i;&c_je;&c_n;&c_t;&c_k;&c_a;</trd>
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_p;&c_a;&c_c;&c_y;&c_je;&c_n;&c_t;</trd>,
        <trd>&c_h;&c_v;&c_o;&c_r;&c_y;</trd>
      </trdgrp>
      <trd lng="de">Patient</trd>
      <trd lng="en">patient</trd>
      <trdgrp lng="fr">
        <trd>patient</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>p&aacute;ciens</trd>,
        <trd>beteg</trd>
      </trdgrp>
      <trd lng="nl">pati&euml;nt</trd>
      <trdgrp lng="ru">
        <trd>&c_p;&c_a;&c_c;&c_i;&c_je;&c_n;&c_t;</trd>,
        <trd>&c_b;&c_o;&c_l;&c_mol;&c_n;&c_o;&c_j;<klr> (&c_v; &c_z;&c_n;.
&c_s;&c_u;&c_shch;.)</klr></trd>
      </trdgrp>
    </drv>
  </art>
