<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>protekt</rad>/i
</kap>

<drv mrk="protek.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>

  <snc mrk="protek.0i.patroni">
    <dif>
      Helpi iun per sia influo kaj a&ubreve;toritato:
      <ekz>
        kiun re&gcirc;o <tld/>as, tiun ministro elektas<fnt>Z</fnt>;
      </ekz><ekz>
        ni ne estas tiaj homoj, kiuj <tld/>us sian filinon en
        malbonaj faroj<fnt>Z</fnt>;
      </ekz><ekz>
        <tld/>i iun al ofico<fnt>B</fnt>;
      </ekz><ekz>
        ne rifuzu al ni vian <tld/>adon<fnt>Z</fnt>;
      </ekz><ekz>
        &scirc;i diris kun <tld/>a tono<fnt>Z</fnt>;
      </ekz><ekz>
        kun afekte <tld/>ema sintenado<fnt>Z</fnt>;
      </ekz><ekz>
        la re&gcirc;o prenis sur sin la <tld/>antecon super la kongreso<fnt>Z</fnt>.
      </ekz> 
    </dif>
    <refgrp tip="sin">
      <ref cel="patron.0i">patroni</ref>.
    </refgrp>
    <trd lng="ru">&c_p;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>

  <snc mrk="protek.0i.sxirmi">
    <dif>
      Defendi, gardi kontra&ubreve; minaco:
      <ekz>
        unu malamiko pli difektas ol cent amikoj <tld/>as<fnt>Z</fnt>;
      </ekz><ekz>
        estu mia genio <tld/>anta kontra&ubreve; tiu &ccirc;i fremdulo<fnt>Z</fnt>;
      </ekz><ekz>
        <tld/>i tiajn trezorojn kontra&ubreve; la detruo.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="sxirm.0i">&scirc;irmi</ref>,
      <ref cel="defend.0i">defendi</ref>
    </refgrp>
    <refgrp tip="vid"> 
      <ref cel="cxirka.0i">&ccirc;irka&ubreve;i</ref>,
      <ref cel="flug.0ilo">flugilo</ref>,
      <ref cel="ombr">ombro</ref>,
      <ref cel="rempar.0o">remparo</ref>,
      <ref cel="sxild.0o">&scirc;ildo</ref>,
      <ref cel="ten">teni sub</ref>
    </refgrp>
    <trd lng="ru">&c_z;&c_a;&c_shch;&c_i;&c_shch;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="de">(be)sch&uuml;tzen</trd>
  <trd lng="en">protect</trd>
  <trd lng="nl">beschermen</trd>
</drv>

<drv mrk="protek.0ado">
  <kap><tld/>(ad)o</kap>
  <dif>
    Ago <tld/>i: 
  </dif>
  <snc>
    <ekz>
      Al <tld/>o kaj forto helpas la sorto<fnt>Z</fnt>.
    </ekz>
  </snc>
  <snc>
    <ekz>
      La <tld/>ado al la malfortuloj estas la devo de &ccirc;iu vera homo. 
    </ekz>
  </snc>
  <trd lng="nl">bescherming</trd>
  <trd lng="ru">&c_z;&c_a;&c_shch;&c_i;&c_t;&c_a;</trd>
</drv>

<drv mrk="protek.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      Ekonomia doktrino, metanta altajn doganojn sur importa&jcirc;ojn
      por <tld/>i la enlandan industrion a&ubreve; komercon. 
    </dif>
    <refgrp tip="sin">
      <ref cel="liber">komerca libereco</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Protektionismus</trd>
  <trd lng="nl">protectionisme</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_je;&c_k;&c_c;&c_i;&c_o;&c_n;&c_i;&c_z;&c_m;</trd>
</drv>

</art>
