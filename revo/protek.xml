<drv mrk="protek.0a">
  <kap><tld/>a</kap>
  <snc mrk="protek.0a.KOMUNE">
    <dif>
      Kiu <tld/>as, helpas kontra&ubreve; dan&gcirc;eroj kaj minacoj:
      <ekz>
        <klr>&scirc;i</klr> diris kun <tld/>a tono
        <fnt><bib>Fab2</bib>, <lok>malbela anasido</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>a spirito de la domo 
        <fnt>
          <bib>Fab3</bib>
          <lok>Lasta perlo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;in]</klr> forlasis &ccirc;io, kio <klr>[&#8230;]</klr> &gcirc;is nun estis por &scirc;i amika kaj <tld/>a
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la ma&scirc;inindustria parto de la &ccirc;ina medi<tld/>a sektoro teknike ankora&ubreve; restas sur tre
        malalta nivelo
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="&Steloj;monato2/007256.html">Mediprotekta industrio
          havas esperigan estonton</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <ind>Natur<tld/>a Monda Fonda&jcirc;o</ind>
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/006912.html">Poluado: 
         Cinike cianide &dash; &ccirc;u respondecas Okcidento?</url></vrk>
        </fnt>;
        <trd lng="de">WWF</trd>
      </ekz>
      <ekz>
        <ind>la infan<tld/>a organiza&jcirc;o de Unui&gcirc;intaj Nacioj</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Christian Fabossou</aut>
          <vrk><url ref="&Steloj;monato2/007037.html">Afriko: Sklavoj &dash;
          kvinjara&gcirc;aj</url></vrk>
        </fnt>.
        <trdgrp lng="de">
          <trd>UNICEF</trd>,
          <trd>Kinderhilfswerk der Vereinten Nationen</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_a;&c_r;&c_o;&c_n;&c_ch;&c_y;</trd>,
    <trd>&c_a;&c_h;&c_o;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
  <trd>sch&uuml;tzend</trd>,
  <trd>Schutz-</trd>
  </trdgrp>
  <trd lng="es">protector</trd>
  <trd lng="fr">protecteur<klr tip="amb"> (adj.)</klr></trd>
  <trd lng="nl">beschermend</trd>
  <trd lng="pl">ochronny</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_shch;&c_i;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_o;&c_h;&c_r;&c_a;&c_n;&c_ja;&c_ju;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_o;&c_h;&c_r;&c_a;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_s;&c_t;&c_u;&c_p;&c_n;&c_i;&c_c;&c_mol;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_o;&c_p;&c_ib;&c_k;&c_u;&c_n;&c_s;&c_mol;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_t;&c_je;&c_k;&c_c;&c_ib;&c_o;&c_n;&c_ib;&c_s;&c_t;&c_s;&c_mol;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
