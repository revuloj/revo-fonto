<drv mrk="protek.0a">
  <kap><tld/>a</kap>
  <snc mrk="protek.0a.KOMUNE">
    <dif>
      Kiu <tld/>as, helpas kontra&ubreve; dan&gcirc;eroj kaj minacoj:
      <ekz>
        <klr>&scirc;i</klr> diris kun <tld/>a tono
        <fnt><bib>Fab2</bib>, <lok>malbela anasido</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>a spirito de la domo 
        <fnt>
          <bib>Fab3</bib>
          <lok>Lasta perlo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;in]</klr> forlasis &ccirc;io, kio <klr>[&#x2026;]</klr> &gcirc;is nun estis por &scirc;i amika kaj <tld/>a
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la ma&scirc;inindustria parto de la &ccirc;ina medi<tld/>a sektoro teknike ankora&ubreve; restas sur tre
        malalta nivelo
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="&Steloj;monato2/007256.html">Mediprotekta industrio
          havas esperigan estonton</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <ind>Natur<tld/>a Monda Fonda&jcirc;o</ind>
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/006912.html">Poluado: 
         Cinike cianide &dash; &ccirc;u respondecas Okcidento?</url></vrk>
        </fnt>;
        <trd lng="de">WWF</trd>
      </ekz>
      <ekz>
        <ind>la infan<tld/>a organiza&jcirc;o de Unui&gcirc;intaj Nacioj</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Christian Fabossou</aut>
          <vrk><url ref="&Steloj;monato2/007037.html">Afriko: Sklavoj &dash;
          kvinjara&gcirc;aj</url></vrk>
        </fnt>.
        <trdgrp lng="de">
          <trd>UNICEF</trd>,
          <trd>Kinderhilfswerk der Vereinten Nationen</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_a;&c_r;&c_o;&c_n;&c_ch;&c_y;</trd>,
    <trd>&c_a;&c_h;&c_o;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
  <trd>sch&uuml;tzend</trd>,
  <trd>Schutz-</trd>
  </trdgrp>
  <trd lng="es">protector</trd>
  <trd lng="fr">protecteur<klr tip="amb"> (adj.)</klr></trd>
  <trd lng="nl">beschermend</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_shch;&c_i;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_o;&c_h;&c_r;&c_a;&c_n;&c_ja;&c_ju;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_o;&c_h;&c_r;&c_a;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x437;&#x430;&#x441;&#x442;&#x443;&#x43F;&#x43D;&#x438;&#x446;&#x44C;&#x43A;&#x438;&#x439;</trd>,
    <trd>&#x43E;&#x43F;&#x456;&#x43A;&#x443;&#x43D;&#x441;&#x44C;&#x43A;&#x438;&#x439;</trd>,
    <trd>&#x43F;&#x440;&#x43E;&#x442;&#x435;&#x43A;&#x446;&#x456;&#x43E;&#x43D;&#x456;&#x441;&#x442;&#x441;&#x44C;&#x43A;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="protek.0o">
  <kap><tld/>o</kap>
  <snc mrk="protek.0o.KOMUNE">
    <dif>
      Subteno, helpo kontra&ubreve; dan&gcirc;eroj kaj eblaj misoj:
      <ekz>
        al <tld/>o kaj forto helpas la sorto
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la Eternulo, Dio de Izrael, sub kies flugiloj vi ser&ccirc;is
        <tld/>on
        <fnt><bib>MT</bib>, <lok>&Rut; 2:12</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> donis al &scirc;i <tld/>on kaj &scirc;irmon &gcirc;is &scirc;ia morto
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> sin rekomendis al ilia <tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de la limoj 
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o kontra&ubreve; bankroto
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Steloj;monato2/007334.html">Belgio:
          Lernout kaj Hauspie: revo fini&gcirc;is</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o kontra&ubreve; atakoj
        <fnt>
          <bib>Monato</bib>
          <aut>Sincere via Stefan Maul</aut>
          <vrk><url ref="&Monato;/2003/008116.html">Milit-litoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_a;&c_r;&c_o;&c_n;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>h&aacute;jen&iacute;</trd>,
    <trd>ochrana</trd>
  </trdgrp>
  <trd lng="de">Schutz</trd>
  <trd lng="es">protecci&oacute;n</trd>
  <trd lng="fr">protection</trd>
  <trdgrp lng="hu">
    <trd>t&aacute;mogat&aacute;s</trd>,
    <trd>v&eacute;delem</trd>
  </trdgrp>
  <trd lng="nl">bescherming</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_shch;&c_i;&c_t;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_t;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ochrana</trd>,
    <trd>protekcia</trd>,
    <trd>z&aacute;&scaron;tita</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x437;&#x430;&#x441;&#x442;&#x443;&#x43F;&#x43D;&#x438;&#x446;&#x442;&#x432;&#x43E;</trd>,
    <trd>&#x43E;&#x445;&#x43E;&#x440;&#x43E;&#x43D;&#x430;</trd>,
    <trd>&#x43E;&#x43F;&#x456;&#x43A;&#x430;</trd>,
    <trd>&#x43F;&#x440;&#x43E;&#x442;&#x435;&#x43A;&#x446;&#x456;&#x44F;</trd>,
