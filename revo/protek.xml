<drv mrk="protek.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="protek.0i.patroni">
    <dif>
      Helpi iun per sia influo kaj a&ubreve;toritato:
      <ekz>
        kiun re&gcirc;o <tld/>as, tiun ministro elektas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ni ne estas tiaj homoj, kiuj <tld/>us sian filinon en
        malbonaj faroj
        <fnt>
          <aut>Jean-Baptiste Moli&egrave;re, trad. L. L. Zamenhof </aut>
          <vrk><url ref="https://www.scribd.com/document/47176682/Georgo-Dandin">Georgo
          Dandin</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> en neniu afero protektis iun al la filo <klr>(la faraono)</klr>
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        morala sistemo volas <tld/>i la vivon
        <fnt>
          <bib>Monato</bib>
          <aut>Reinhard Haupenthal</aut>
          <vrk><url ref="&Steloj;monato2/006848.html">Mallogika kaj
          senracia</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="patron.0i">patroni</ref>
    </refgrp>
    <trdgrp lng="hu">
      <trd>t&aacute;mogat</trd>,
      <trd>seg&iacute;t</trd>,
      <trd>protezs&aacute;l</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="protek.0i.sxirmi">
    <dif>
      Defendi, gardi kontra&ubreve; minaco:
      <ekz>
        unu malamiko pli difektas ol cent amikoj <tld/>as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        vundan lokon <tld/>is, alian difektis
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kovra&jcirc;eto por <tld/>i ilin kontra&ubreve; la malvarmeco
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
      <ekz>
        permane <klr>[&#x2026;]</klr> protekti sin de l' vid'
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i trezorojn kontra&ubreve; la detruo;
      </ekz>
      <ekz>
        mi suferas, protektu min
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 38:14</lok>
        </fnt>!
      </ekz>
      <ekz>
        pilolo &dash; kiu ne <tld/>as kontra&ubreve; aidoso
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato1/monato-9501-seksaj-rilatoj.txt">Seksaj
          Rilatoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        neniu efektive lin <tld/>is, male oni traktis lin <klr>[&#x2026;]</klr> kvaza&ubreve; li estus pleja
        krimulo
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="&Steloj;monato2/006823.html">Opinio: Tro Senkosta
          Pentemo</url></vrk>
        </fnt>.
      </ekz>
     </dif>
    <refgrp tip="sin">
      <ref cel="sxirm.0i.protekti">&scirc;irmi <sncref/></ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="defend.0i">defendi</ref>,
      <ref cel="flug.0ilo">flugilo</ref>,
      <ref cel="rempar.0o">remparo</ref>,
      <ref cel="sxild.0o">&scirc;ildo</ref>,
      <ref cel="ten">teni sub</ref>
    </refgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_shch;&c_i;&c_shch;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_je;&c_d;&c_o;&c_h;&c_r;&c_a;&c_n;&c_ja;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
