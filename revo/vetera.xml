<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vetera.xml,v 1.26 2024/06/09 23:25:35 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>veteran</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="vetera.0o">
  <kap><tld/>o</kap>
  <snc mrk="vetera.0o.MIL">
    <dif>
      Longe servinta soldato:
      <ekz>
        kvankam li estis nova al la trupo, li ne estis novico; male, li
        estis <tld/>o jam plenuminta kompletan serion da operacioj
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="vetera.0o.FIG">
    <dif>
      Longe servinta oficisto.
    </dif>
    <refgrp tip="vid">
      <ref cel="ekzerc">ekzercito</ref>,
      <ref cel="emerit.0o.POL">emerito</ref>,
      <ref cel="spert">spertulo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_e;&c_t;&c_e;&c_r;&c_a;&c_n;</trd>
  <trd lng="ca">veter&agrave;</trd>
  <trdgrp lng="cs">
    <trd><klr>(zaslou&zcaron;il&yacute;)</klr> pracovn&iacute;k na odpo&ccaron;inku</trd>,
    <trd>veter&aacute;n</trd>,
    <trd>vyslou&zcaron;ilec</trd>
  </trdgrp>
  <trd lng="de">Veteran</trd>
  <trd lng="en">veteran</trd>
  <trd lng="es">veterano<klr> (subst.)</klr></trd>
  <trd lng="fr">v&eacute;t&eacute;ran</trd>
  <trd lng="hu">veter&aacute;n</trd>
  <trdgrp lng="ja">
    <trd>&#x719F;&#x7DF4;&#32773;  <pr>&#12376;&#12421;&#x304F;&#x308C;&#12435;&#12375;&#12419;</pr></trd>,
    <trd>&#x30D9;&#x30C6;&#x30E9;&#x30F3;  <pr>&#12409;&#12390;&#12425;&#12435;</pr></trd>,
    <trd>&#x53E4;&#x53C2;&#20853;  <pr>&#12371;&#12373;&#12435;&#12408;&#12356;</pr></trd>,
    <trd>&#36864;&#x5F79;&#x8ECD;&#x4EBA;  <pr>&#x305F;&#12356;&#12360;&#x304D;&#12368;&#12435;&#12376;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">veteraan</trd>
  <trdgrp lng="pl">
    <trd>weteran</trd>,
    <trd>wiarus</trd>
  </trdgrp>
  <trd lng="pt">veterano</trd>
  <trd lng="ru">&c_v;&c_je;&c_t;&c_je;&c_r;&c_a;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>star&yacute; pracovn&iacute;k kolekt&iacute;vu</trd>,
    <trd>veter&aacute;n</trd>,
    <trd>vojensk&yacute; vysl&uacute;&zcaron;ilec</trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_je;&c_t;&c_je;&c_r;&c_a;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x69AE;&#x6C11; <pr>r&#xF3;ngm&#xED;n</pr></trd>,
    <trd>&#33635;&#x6C11; <pr>r&#xF3;ngm&#xED;n</pr></trd>,
    <trd>&#32769;&#20853; <pr>l&#x1CE;ob&#x12B;ng</pr></trd>,
    <trd>&#36864;&#x4F0D;&#x519B;&#x4EBA; <pr>tu&#xEC;w&#x1D4;j&#x16B;nr&#xE9;n</pr></trd>,
    <trd>&#36864;&#x4F0D;&#x8ECD;&#x4EBA; <pr>tu&#xEC;w&#x1D4;j&#x16B;nr&#xE9;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="vetera.0a">
  <kap><tld/>a</kap>
  <snc mrk="vetera.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kiu havas longan sperton en la koncerna afero;
      ne <ref tip="ant" cel="novic.0a.FIG">novica</ref>:
      <ekz>
        la direktadon prizorgas la <tld/>a samideano D-ro Anakreon
        Stamadiadis
        <fnt>
          <aut>Verdano</aut>,
          <vrk>El nia Gazetaro</vrk>,
          <lok>The British Esperantist, 1932, p. 180a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_e;&c_t;&c_e;&c_r;&c_a;&c_n;&c_s;&c_k;&c_ib;</trd>,
    <trd>&c_d;&c_a;&c_w;&c_n;&c_ib;</trd>
  </trdgrp>
  <trd lng="es">veterano<klr> (adj.)</klr></trd>
  <trd lng="fr">v&eacute;t&eacute;ran<klr> (adj.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#32769;&#x7DF4;&#x306A;  <pr>&#x308D;&#12358;&#x308C;&#12435;&#x306A;</pr></trd>,
    <trd>&#38263;&#x304F;&#x7D4C;&#x9A13;&#12434;&#x7A4D;&#12435;&#12384;  <pr>&#x306A;&#x304C;&#x304F;&#12369;&#12356;&#12369;&#12435;&#12434;&#12388;&#12435;&#12384;</pr></trd>,
    <trd>&#x30D9;&#x30C6;&#x30E9;&#x30F3;&#x306E;  <pr>&#12409;&#12390;&#12425;&#12435;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wys&lstroke;u&zdot;ony</trd>,
    <trd>do&sacute;wiadczony</trd>
  </trdgrp>
</drv>
</art>
