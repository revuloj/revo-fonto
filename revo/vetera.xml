<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>veteran</rad>/o
</kap>
<drv mrk="vetera.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Longe servinta soldato.
    </dif>
    <refgrp tip="vid">
      <ref cel="hard.0i">hardi</ref>
    </refgrp>
  </snc>
  <snc>
    <dif>
      Longe servinta oficisto.
    </dif>
    <refgrp tip="sin">
      <ref cel="spert">spertulo</ref>,
      <ref cel="ekzerc">ekzercito</ref>,
      <ref cel="emerit">emerita</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_e;&c_t;&c_e;&c_r;&c_a;&c_n;</trd>
  <trd lng="ca">veter&agrave;</trd>
  <trd lng="de">Veteran</trd>
  <trd lng="en">veteran</trd>
  <trd lng="fr">v&eacute;t&eacute;ran</trd>
  <trd lng="hu">veter&aacute;n</trd>  
  <trd lng="nl">veteraan</trd>
  <trd lng="pt">veterano</trd>
  <trd lng="ru">&c_v;&c_je;&c_t;&c_je;&c_r;&c_a;&c_n;</trd>
  <trd lng="sk">veter&aacute;n</trd>
</drv>
</art>
