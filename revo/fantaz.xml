<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>fantazi</rad>/o
</kap>
<drv mrk="fantaz.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Krea imagpovo: <ekz>tio flamigis &scirc;ian infanan <tld/>on.</ekz>
    </dif>
    <trdgrp lng="de">
     <trd>Einbildungskraft</trd>,
     <trd>Vorstellungskraft</trd>,
     <trd>Fantasie</trd>,
     <trd>Phantasie</trd></trdgrp>
    <trdgrp lng="nl">
    <trd>fantasie</trd>,
    <trd>verbeeldingskracht</trd>
  </trdgrp>
<trdgrp lng="ru">
      <trd>&c_f;&c_a;&c_n;&c_t;&c_a;&c_z;&c_i;&c_ja;</trd>,
      <trd>&c_v;&c_o;&c_o;&c_b;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <dif>
      Kaprica sendirekta kaj senregula vagado de la imago:
      <ekz>li &ccirc;iam havas iujn <tld/>ojn<fnt>Z</fnt>.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekstra">ekstravaganco</ref>.
    </refgrp>
    <trdgrp lng="de">
     <trd>Einbildung</trd>,
     <trd>Fantasie</trd>,
     <trd>Phantasie</trd></trdgrp>
    <trdgrp lng="nl">
    <trd>fantasie</trd>,
    <trd>inbeelding</trd>
  </trdgrp>
<trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_i;&c_h;&c_o;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_t;&c_je;&c_ja;</trd>,
      <trd>&c_f;&c_a;&c_n;&c_t;&c_a;&c_z;&c_i;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc num="3">
    <dif>
      Kaprica vagado de la imago ekster la realo kaj eblo:
      <ekz>oni parolis pri tio kiel pri ia dol&ccirc;a <tld/>o<fnt>Z</fnt>;</ekz>
      <ekz>senenhava <tld/>o<fnt>Z</fnt>.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="delir.0o">deliro</ref>,
      <ref cel="iluzi.0o">iluzio</ref>,
      <ref cel="hximer.0o">&hcirc;imero</ref>,
      <ref cel="rev">revo</ref>,
      <ref cel="songx.0o">son&gcirc;o</ref>,
      <ref cel="utopi.0o">utopio</ref>,
      <ref cel="fabel.0o">fabelo</ref>,
      <ref cel="mit.0o">mito</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="aer.0o">aero</ref>.
    </refgrp>
    <trdgrp lng="de">
     <trd>Spinnerei</trd>,
     <trd>Phantasterei</trd></trdgrp>
    <trd lng="nl">verbeelding</trd>
    <trd lng="ru">&c_f;&c_a;&c_n;&c_t;&c_a;&c_z;&c_i;&c_ja;</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>fantaisie</trd>,
    <trd>imagination</trd>,
    <trd>invention</trd>,
    <trd>lubie</trd></trdgrp> 
</drv>
<drv mrk="fantaz.0a">
  <kap><tld/>a</kap>
  <snc num="1">
    <dif>
      Imagoplena. 
    </dif>
  <trd lng="nl">fantasierijk</trd>
  </snc>
  <snc num="2">
    <dif>
      Kaprica, sendirekta, senregula:
      <ekz>&scirc;i plektis <tld/>ajn kronojn el floroj;</ekz>
      <ekz><tld/>a vesto.</ekz>
    </dif>
    <trd lng="nl">fantasievol</trd>
   <trd lng="ru">&c_p;&c_r;&c_i;&c_h;&c_o;&c_t;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
  </snc>
  <snc num="3">
    <dif>
      Nereala, senbaza, nur imagita:
      <ekz><tld/>a rakonto;</ekz>
      <ekz>tio estas ne ia <tld/>a revo, sed apero tute natura<fnt>Z</fnt>.
      </ekz> 
    </dif>
    <trdgrp lng="nl">
    <trd>ingebeeld</trd>,
    <trd>gefantaseerd</trd>
  </trdgrp>
<trdgrp lng="ru">
      <trd>&c_f;&c_a;&c_n;&c_t;&c_a;&c_s;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_v;&c_y;&c_d;&c_u;&c_m;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>de <ind>fantaisie</ind></trd>,
    <trd>fictif<klr tip="ind"> (imagin&eacute;)</klr></trd>,
    <trd>fantastique<klr tip="ind"> (produit de l'imagination)</klr></trd>,
    <trd>imagin&eacute;</trd>,
    <trd>imaginaire<klr tip="ind"> (invent&eacute;)</klr></trd>
    </trdgrp> 
</drv>
<drv mrk="fantaz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1">
    <dif>
      Kaprice imagi: <ekz><tld/>i strangan rakonton.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="prezen">prezenti al si</ref>.
    </refgrp>
  </snc>
  <snc num="2">
    <dif>
      <tld lit="F"/>e verki, fari: <ekz>amuza sceno <tld/>ita de Aristofano.</ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>inventer<klr tip="ind"> (qc de fantastique)</klr></trd>,
    <trd>imaginer<klr tip="ind"> (qc de fantastique)</klr></trd></trdgrp> 
  <trd lng="nl">fantaseren</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_a;&c_n;&c_t;&c_a;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_m;&c_je;&c_ch;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_o;&c_o;&c_b;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_d;&c_u;&c_m;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="fantaz.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Homo tre kaprice kaj strange aganta. 
    </dif>
    <refgrp tip="vid">
      <ref cel="strang">strangulo</ref>,
      <ref cel="origin1.0ulo">originalulo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>original<klr tip="ind"> (personne bizare)</klr></trd>,
    <trd>ph&eacute;nom&egrave;ne<klr tip="ind"> (personne bizare)</klr></trd>,
    <trd>fantaisiste</trd></trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_f;&c_a;&c_n;&c_t;&c_a;&c_z;&c_jo;&c_r;</trd>,
    <trd>&c_m;&c_je;&c_ch;&c_t;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
</drv>
</art>

