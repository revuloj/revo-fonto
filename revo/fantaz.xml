</kap>
<drv mrk="fantaz.0o">
  <kap><tld/>o</kap>
  <snc mrk="fantaz.0o.PSI">
    <dif>
      Krea imagpovo:
      <ekz>
        tio flamigis &scirc;ian infanan <tld/>on.
      </ekz>
      <ekz>
        al E&ubreve;ropo, <klr>[&#8230;]</klr> al la lando de la prapatroj, al la sankta lando de la
        rememoroj kaj de la <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Post jarmiloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[por]</klr> e&ccirc; el kolbasaj bastonetoj <klr>[&#8230;]</klr> kuiri supon
        <klr>[&#8230;]</klr> oni bezonas tri aferojn: sa&gcirc;on, <tld/>on kaj senton
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        preska&ubreve; fulme en lia fajra <tld/>o ordi&gcirc;is la nova&jcirc;oj kaj formi&gcirc;is plano
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Einbildungskraft</trd>,
      <trd>Vorstellungskraft</trd>,
      <trd>Fantasie</trd>,
      <trd>Phantasie</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>fant&aacute;zia</trd>,
      <trd>k&eacute;pzel&odblac;er&odblac;</trd>
    </trdgrp>
	<trdgrp lng="it">
      <trd>fantasia</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>fantasie</trd>,
      <trd>verbeeldingskracht</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_f;&c_a;&c_n;&c_t;&c_a;&c_z;&c_i;&c_ja;</trd>,
      <trd>&c_v;&c_o;&c_o;&c_b;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x82B1;&#x4FCF; <pr>hu&amacron;qi&#xE0;o</pr></trd>,
    <trd>&#x5E7B;&#x60F3;&#x529B; <pr>hu&#xE0;nxi&#x1CE;ngl&#xEC;</pr></trd>,
    <trd>&#x865A;&#x5E7B; <pr>x&#x16B;hu&#xE0;n</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="fantaz.0o.fantaziajxo">
    <dif>
      <ref tip="dif" cel="fantaz.0ajxo.PSI">Fantazia&jcirc;o</ref>:
      <ekz>
        tiaj ideoj, <klr>[&#8230;]</klr> al la samtempuloj &scirc;ajnas senenhava <tld/>o kaj al la
        posteuloj &scirc;ajnas <klr>[&#8230;]</klr> natura afero
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;io &ccirc;i tio estas <tld/>oj kaj apartenas al la tiel nomata nigra magio
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Helga</nom>n tre ofte kaptadis la malbona <tld/>o sidi&gcirc;i sur la rando de la puto, bati
        &ccirc;irka&ubreve;en <klr>[&#8230;]</klr> kaj poste &jcirc;eti sin en la mallar&gcirc;an profundan puton
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="delir.0o">deliro</ref>,
    <ref cel="ekstra1.0o">ekstravaganco</ref>,
    <ref cel="ide.0o">ideo</ref>,
    <ref cel="fabel.0o">fabelo</ref>,
    <ref cel="hximer.0o">&hcirc;imero</ref>,
    <ref cel="iluzi.0o">iluzio</ref>,
    <ref cel="rev.0o">revo</ref>,
    <ref cel="songx.0o">son&gcirc;o</ref>,
    <ref cel="utopi.0o">utopio</ref>.
  </refgrp>
  <trd lng="be">&c_f;&c_a;&c_n;&c_t;&c_a;&c_z;&c_ib;&c_ja;</trd>
  <trd lng="bo">&#xF51;&#xF58;&#xF72;&#xF42;&#xF66;&#xF0B;&#xF66;&#xFA3;&#xF44;&#xF0B;</trd>
<trdgrp lng="en">
  <trd>phantasy (brite)</trd>,
  <trd>fantasy (usone)</trd>
</trdgrp>
  <trd lng="es">fantas&iacute;a</trd>
  <trdgrp lng="fr">
    <trd>fantaisie</trd>,
    <trd>imagination</trd>,
    <trd>invention</trd>,
    <trd>lubie</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5E7B;&#x60F3;  <pr>&#12370;&#12435;&#x305D;&#12358;</pr></trd>,
    <trd>&#22818;&#x60F3;  <pr>&#12416;&#x305D;&#12358;</pr></trd>,
    <trd>&#x7A7A;&#x60F3;&#x529B;  <pr>&#x304F;&#12358;&#x305D;&#12358;&#x308A;&#12423;&#x304F;</pr></trd>,
    <trd>&#x5E7B;&#x60F3;&#x66F2;  <pr>&#12370;&#12435;&#x305D;&#12358;&#x304D;&#12423;&#x304F;</pr></trd>,
    <trd>&#x30D5;&#x30A1;&#x30F3;&#x30BF;&#x30B8;&#x30A2;</trd>
  </trdgrp>
  <trd lng="pt">fantasia</trd>
  <trd lng="sk">obrazotvornos&tcaron;</trd>
  <trdgrp lng="cs">
    <trd>fantazie</trd>,
    <trd>obrazotvornost</trd>,
    <trd>p&rcaron;edstavivost</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_f;&c_a;&#x43D;&c_t;&c_a;&c_z;&c_ib;&#x44F;</trd>,
    <trd>&c_u;&#x44F;&c_v;&c_a;</trd>,
    <trd>&#x43F;&c_r;&c_i;&#x43C;&c_h;&c_a;</trd>,
    <trd>&c_v;&c_i;&c_t;&c_ib;&c_v;&#x43A;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="fantaz.0a">
  <kap><tld/>a</kap>
  <snc mrk="fantaz.0a.PSI">
    <dif>
      Imagoplena.
    </dif>
    <subsnc mrk="fantaz.0a.kaprica">
      <dif>
        Kaprica, sendirekta, senregula:
        <ekz>
          &scirc;i plektis <tld/>ajn kronojn el floroj
          <fnt>
            <bib>Hamlet</bib>
          </fnt>;
        </ekz>
        <ekz>
          <tld/>a vesto;
        </ekz>
        <ekz>
          vastega palaco kun unu <tld/>a kolonaro super la alia
          <fnt>
            <bib>Fab1</bib>
            <lok>Sova&gcirc;aj cignoj</lok>
          </fnt>;
        </ekz>
        <ekz>
          kia kaverno tio estis! la grandaj &scirc;tonblokoj, de kiuj gutadis la akvo, pendis
          super ili en plej <tld/>aj formoj
          <fnt>
            <bib>Fab1</bib>
            <lok>&Gcirc;ardeno de la paradizo</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="nl">fantasievol</trd>
      <trd lng="ru">&c_p;&c_r;&c_i;&c_h;&c_o;&c_t;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
    </subsnc>
    <subsnc mrk="fantaz.0a.malreala">
      <dif>
        Nereala, senbaza, nur imagita:
        <ekz>
          <tld/>a rakonto;
        </ekz>
        <ekz>
          afero, kiu ne sole havas grandegan gravecon por la homaro, sed kiu al tio havas
          en si nenion <tld/>an
          <fnt>
            <bib>FK</bib>
          </fnt>;
        </ekz>
        <ekz>
          mi ripete turnas la okulojn <klr>[&#8230;]</klr> sur tiun &ccirc;i punkton, &scirc;ajne <tld/>an,
          sed efektive tre gravan
          <fnt>
            <bib>DL</bib>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="nl">
        <trd>ingebeeld</trd>,
        <trd>gefantaseerd</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_f;&c_a;&c_n;&c_t;&c_a;&c_s;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
        <trd>&c_v;&c_y;&c_d;&c_u;&c_m;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
      </trdgrp>
    </subsnc>
    <trd lng="nl">fantasierijk</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_n;&c_t;&c_a;&c_z;&c_ib;&c_j;&c_n;&c_y;</trd>,
    <trd>&c_f;&c_a;&c_n;&c_t;&c_a;&c_s;&c_t;&c_y;&c_ch;&c_y;</trd>,
    <trd>&c_v;&c_y;&c_d;&c_u;&c_m;&c_a;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>fantasievoll</trd>,
    <trd>fantastisch</trd>,
    <trd>ertr&auml;umt</trd>
  </trdgrp>
<trdgrp lng="en">
  <trd>fantastic</trd>,
  <trd>fantasy</trd>
</trdgrp>
  <trdgrp lng="es">
    <trd>fant&aacute;stico</trd>,
    <trd>fantasioso</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>de <ind>fantaisie</ind></trd>,
    <trd>fictif<klr tip="ind"> (imagin&eacute;)</klr></trd>,
    <trd>fantastique<klr tip="ind"> (produit de
    l'imagination)</klr></trd>,
    <trd>imagin&eacute;</trd>,
    <trd>imaginaire<klr tip="ind"> (invent&eacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fantasztikus</trd>,
    <trd>k&eacute;pzelt</trd>,
    <trd>&aacute;br&aacute;ndos</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>fantastico</trd>,
    <trd>fantasioso</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5E7B;&#x60F3;&#30340;&#x306A;  <pr>&#12370;&#12435;&#x305D;&#12358;&#12390;&#x304D;&#x306A;</pr></trd>,
    <trd>&#12392;&#x308A;&#12392;&#12417;&#x306E;&#x306A;&#12356;</trd>,
    <trd>&#x6C17;&#x307E;&#12368;&#x308C;&#x306A;  <pr>&#x304D;&#x307E;&#12368;&#x308C;&#x306A;</pr></trd>,
    <trd>&#22868;&#x653E;&#x306A;  <pr>&#x307B;&#12435;&#x307D;&#12358;&#x306A;</pr></trd>,
    <trd>&#x7A7A;&#x60F3;&#30340;&#x306A;  <pr>&#x304F;&#12358;&#x305D;&#12358;&#12390;&#x304D;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_f;&c_a;&#x43D;&c_t;&c_a;&c_s;&c_t;&c_i;&c_ch;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_i;&c_g;&c_a;&c_d;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="zh">&#26377;&#x60F3;&#x8C61;&#x529B; <pr>y&#x1D2;uxi&#x1CE;ngxi&#xE0;ngl&#xEC;</pr></trd>
</drv>

<drv mrk="fantaz.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="fantaz.0ajxo.PSI">
    <dif>
      Kaprica imaga&jcirc;o:
    </dif>
    <subsnc mrk="fantaz.0ajxo.kaprica">
      <dif>
        Kaprica sendirekta kaj senregula vagado de la imago.
      </dif>
      <trd lng="en">fantasy</trd>
      <trdgrp lng="de">
        <trd>Einbildung</trd>,
        <trd>Fantasie</trd>,
        <trd>Phantasie</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>k&eacute;pzel&odblac;d&eacute;s</trd>,
        <trd>fant&aacute;zi&aacute;l&aacute;s</trd>
      </trdgrp>
      <trdgrp lng="it">
        <trd>fantasia</trd>,
        <trd>chimera</trd>,
        <trd>sogno</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>fantasie</trd>,
        <trd>inbeelding</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_p;&c_r;&c_i;&c_h;&c_o;&c_t;&c_mol;</trd>,
        <trd>&c_z;&c_a;&c_t;&c_je;&c_ja;</trd>,
        <trd>&c_f;&c_a;&c_n;&c_t;&c_a;&c_z;&c_i;&c_ja;</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="fantaz.0ajxo.malreala">
      <dif>
        Vagado de la imago ekster la realo kaj eblo:
        <ekz>
          tezo, ke la katastrofaj naturfenomenoj lige kun la elmigro de la hebrea gento el
          Egiptio <klr>[&#8230;]</klr> ne estas nur <tld/>a&jcirc;oj, sed historiaj eventoj
         <fnt>
        <aut>Manuel Halvelik</aut>
          <vrk><url ref="&Monato;/1993/A931102.php">Kaj la suno haltis ...</url></vrk>
          <bib>Monato</bib>
          <lok>1993/11, p. 23</lok>
        </fnt>;
        </ekz>
        <ekz>
          tiom da anatomiaj faktoj malpravigas tiun &ccirc;i supozon, ke e&ccirc; ne valoras analizi
          tiun &ccirc;i <tld/>a&jcirc;on
          <fnt>
          <aut>Wim de Smet</aut>
          <vrk><url ref="&Monato;/2000/006900.php">La &leftquot;drakoj&#x201D; de Komodo:
          Gigantaj bestoj sur izola insulo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 18</lok>
        </fnt>;
        </ekz>
        <ekz>
          la Eliksiro de la Vivo estas <tld/>a&jcirc;o
          <fnt>
            <aut>M. Shelley, trad. K. Lendon</aut>,
            <vrk><url ref="http://www012.upp.so-net.ne.jp/klivo/frankenstein/vol1chapitro02.html">
            Franken&scirc;tajno a&ubreve; la moderna Prometeo</url></vrk>,
            <lok>2014</lok>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>Spinnerei</trd>,
        <trd>Fantasterei</trd>
      </trdgrp>
      <trd lng="hu">&aacute;br&aacute;nd</trd>
      <trd lng="nl">verbeelding</trd>
      <trd lng="ru">&c_f;&c_a;&c_n;&c_t;&c_a;&c_z;&c_i;&c_ja;</trd>
    </subsnc>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_n;&c_t;&c_a;&c_z;&c_ib;&c_ja;</trd>,
    <trd>&c_v;&c_y;&c_d;&c_u;&c_m;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_h;&c_a;&c_m;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_je;&c_sh;&c_t;&c_a; <ind>&c_w;&c_ja;&c_w;&c_n;&c_a;&c_je;</ind></trd>,
    <trd>&c_l;&c_je;&c_t;&c_u;&c_c;&c_je;&c_n;&c_s;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="ja">&#x5E7B;&#x5F71;  <pr>&#12370;&#12435;&#12360;&#12356;</pr></trd>
  <trd lng="zh">&#x767D;&#x65E5;&#22818; <pr>b&#xE1;ir&#xEC;m&#xE8;ng</pr></trd>
</drv>

<drv mrk="fantaz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fantaz.0i.PSI">
    <dif>
      Kaprice imagi, <tld/>e verki, fari:
      <ekz>
        <tld/>i strangan rakonton;
      </ekz>
      <ekz>
        amuza sceno <tld/>ita de Aristofano;
      </ekz>
      <ekz>
        vi <tld/>as, mia kara, vi estas beda&ubreve;rinda
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni prenu, ke mi <tld/>as, ke mi eraras, ke mi tro multe esperas <klr>[&#8230;]</klr>
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        la leganto vidos, ke mi ne <tld/>as
        <fnt>
          <bib>DL</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="prezen">prezenti al si</ref>.
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_n;&c_t;&c_a;&c_z;&c_ib;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_d;&c_u;&c_m;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>fantasieren </trd>,
    <trd>spinnen <klr>(Gedanken)</klr></trd>
  </trdgrp>
  <trd lng="es">fantasear</trd>
  <trdgrp lng="fr">
    <trd>inventer<klr tip="ind"> (qc de fantastique)</klr></trd>,
    <trd>imaginer<klr tip="ind"> (qc de fantastique)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fant&aacute;zi&aacute;l</trd>,
    <trd>k&eacute;pzel&odblac;dik</trd>,
    <trd>&aacute;br&aacute;ndozik</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>fantasticare</trd>,
    <trd>immaginare</trd>,
    <trd>sognare a occhi aperti</trd>
  </trdgrp>
<trdgrp lng="en">
  <trd>fantasize</trd>,
  <trd>imagine</trd>,
  <trd>daydream</trd>
</trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7A7A;&#x60F3;&#12391;&#x4F5C;&#x308A;&#x51FA;&#12377;
    <pr>&#x304F;&#12358;&#x305D;&#12358;&#12391;&#12388;&#x304F;&#x308A;&#12384;&#12377;</pr></trd>,
    <trd>&#12391;&#12387;&#12385;&#x4E0A;&#12370;&#x308B;  <pr>&#12391;&#12387;&#12385;&#12354;&#12370;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">fantaseren</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_a;&c_n;&c_t;&c_a;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_m;&c_je;&c_ch;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_o;&c_o;&c_b;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_d;&c_u;&c_m;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bl&uacute;zni&tcaron;</trd>,
    <trd>vym&yacute;&scaron;&lcaron;a&tcaron; si</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>blouznit</trd>,
    <trd>vym&yacute;&scaron;let si</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_f;&c_a;&#x43D;&c_t;&c_a;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43C;&c_r;&c_ib;&#x44F;&c_t;&c_i;</trd>,
    <trd>&c_u;&#x44F;&c_v;&#x43B;&#x44F;&c_t;&c_i;</trd>,
    <trd>&c_ib;&#x43C;&#x43F;&c_r;&#x43E;&c_v;&c_ib;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i; (&#x43C;&c_u;&c_z;&c_i;&#x43A;&c_u;)</trd>
  </trdgrp>
  <trd lng="zh">&#22818;&#x898B; <pr>m&#xE8;ngji&#xE0;n</pr></trd>
</drv>

<drv mrk="fantaz.0ulo">
  <kap><tld/>ulo, <var><kap><tld/>isto</kap></var></kap>
  <snc mrk="fantaz.0ulo.PSI">
    <dif>
      Homo tre kaprice kaj strange aganta;
      <ref tip="vid" cel="strang">strangulo</ref>,
      <ref tip="vid" cel="origin1.0ulo">originalulo</ref>:
      <ekz>
        ni tute ne estas tiaj <tld/>istoj kaj utopiistoj, kiajn multaj el vi eble vidis
        en ni
        <fnt>
