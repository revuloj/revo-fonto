<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>aktini</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="aktini.0o">
  <kap><tld/>o</kap>
  <snc mrk="aktini.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="anemon.mar0oj.ZOO">maranemono</ref>
    <ekz>
      mi iaokaze babilis pri la nutrado de malgrandaj
      animaloj en maro, kaj rekomendis al mia studento,
      s-ro Ida, provi nutradon de <tld/>oj
      <fnt>
        <aut>&Scirc;. Hori</aut>
        <vrk><url
        ref="http://www.eventoj.hu/steb/biologio/la-volvotigo-de-amikeco.pdf">
        La Volvotigo de Amikeco</url></vrk>
        <lok>1965</lok>
      </fnt>.
    </ekz>
    <trdgrp lng="bg">
      <trd>&c_a;&c_k;&c_t;&c_i;&c_n;&c_i;&c_ja;</trd>,
      <trd>&c_m;&c_o;&c_r;&c_s;&c_k;&c_a; &c_r;&c_o;&c_z;&c_a;</trd>
    </trdgrp>
    <trd lng="cs">mo&rcaron;sk&eacute; sasanky</trd>
    <trd lng="de">Seeanemone</trd>
    <trdgrp lng="en">
      <trd>actinia</trd>,
      <trd>sea anemone</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>actinie</trd>,
      <trd>an&eacute;mone de mer</trd>
    </trdgrp>
    <trd lng="pl">ukwia&lstroke;</trd>
    <trdgrp lng="ru">
      <trd>&c_a;&c_k;&c_t;&c_i;&c_n;&c_i;&c_ja;</trd>,
      <trd>&c_m;&c_o;&c_r;&c_s;&c_k;&c_o;&c_j; &c_a;&c_n;&c_je;&c_m;&c_o;&c_n;</trd>
    </trdgrp>
    <trd lng="sk">morsk&eacute; sasanky</trd>
  </snc>                         
  <snc mrk="aktini.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <ref tip="dif" cel="aktinium.0o">aktiniumo</ref>
    <trd lng="be">&c_a;&c_k;&c_t;&c_y;&c_n;&c_ib;&c_j;</trd>
    <trd lng="bg">&c_a;&c_k;&c_t;&c_i;&c_n;&c_i;&c_j;</trd>
    <trd lng="br">aktiniom</trd>
    <trd lng="ca">actini</trd>
    <trd lng="cs">aktinium</trd>
    <trd lng="de">Actinium</trd>
    <trd lng="en">actinium</trd>
    <trdgrp lng="es">
      <trd>actinia</trd>,
      <trd>an&eacute;mona<klr tip="amb"> (de mar)</klr></trd>
    </trdgrp>
    <trd lng="fr">actinium</trd>
    <trd lng="id">aktinium</trd>
    <trd lng="it">attinio</trd>
    <trd lng="la">Actium</trd>
    <trd lng="nl">actinium</trd>
    <trd lng="pl">aktyn</trd>
    <trd lng="pt">act&iacute;nio</trd>
    <trd lng="sk">akt&iacute;nium</trd>
    <trd lng="sv">aktinium</trd>
  </snc>
</drv>
</art>

