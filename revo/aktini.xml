<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>aktini</rad>/o
</kap>
<drv mrk="aktini.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
     &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
     atomnumero 89, atompezo 227,0278, simbolo Ac. &Ccirc;iuj
     izotopoj de tiu elemento estas radioaktivaj.
    </dif>
  </snc>
  <trd lng="de">Actinium</trd>
  <trd lng="fr">actinium</trd>
  <trd lng="nl">actinium</trd>
  <trd lng="ru">&c_a;&c_k;&c_t;&c_i;&c_n;&c_i;&c_j;</trd>
</drv>
</art>

