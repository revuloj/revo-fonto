<art mrk="$Id: log1.xml,v 1.25 2023/10/19 14:32:17 revo Exp $">
<kap>
  <rad>log</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="log1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Instrumento, konsistanta el &scirc;nuro kun flospeco, kiun oni mergas en
      maron por mezuri la rapidon de &scirc;ipo:
      <ekz>
        Helpiloj de navigado inter alie estas <klr>[&#x2026;]</klr> kompaso,
        <tld/>o, sonaro,
        radaro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=274525">Navigado</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      Ne konfuzu kun <ref tip="hom" cel="log.0o">logo</ref> (tento).
    </rim>
    <bld lok="&WCU;/thumb/1/1c/Schlepplogs.jpg/640px-Schlepplogs.jpg" prm="CC BY 3.0">diverstipaj <tld/>oj
        <fnt>
          <aut>PaterMcFly, propra verko</aut>
          <lok><url ref="&WCW;/File:Schlepplogs.jpg">&WCW;/File:Schlepplogs.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_g; <klr>(&c_v;&c_y;&c_m;&c_ja;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_p;&c_r;&c_y;&c_l;&c_a;&c_d;&c_a;)</klr></trd>
  <trdgrp lng="cs">
    <trd>vnada</trd>,
    <trd>&zcaron;ensk&eacute; vnady</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Log</trd>
  </trdgrp>
  <trd lng="en">log</trd>
  <trdgrp lng="es">
    <trd>barquilla</trd>
  </trdgrp>
  <trd lng="fr">loch</trd>
  <trdgrp lng="hu">
    <trd>sebess&eacute;gm&eacute;r&odblac; <klr tip="amb">(haj&oacute;z&aacute;s)</klr></trd>,
    <trd>log</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x9B45;&#x529B;  <pr>&#x307F;&#x308A;&#x3087;&#x304F;</pr></trd>,
    <trd>&#x9B45;&#x60D1;  <pr>&#x307F;&#x308F;&#x304F;</pr></trd>,
    <trd>&#x8A98;&#x60D1;  <pr>&#x3086;&#x3046;&#x308F;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">log</trd>
  <trd lng="pl">log <klr>(przyrz&aogonek;d)</klr></trd>
  <trd lng="pt">barquinha</trd>
  <trd lng="ro">log <klr>(dispozitiv)</klr></trd>
  <trd lng="ru">&c_l;&c_a;&c_g;</trd>
  <trdgrp lng="sk">
    <trd>vnada</trd>,
    <trd>&zcaron;ensk&eacute; vnady</trd>
  </trdgrp>
</drv>
</art>
