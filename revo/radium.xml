<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: radium.xml,v 1.24 2023/09/03 07:53:08 revo Exp $">
<kap>
  <rad>radium</rad>/o
</kap>

<drv mrk="radium.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Ra</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="88">elemento</ref> kun
      atomnumero 88, atompezo 226,0254; tre 
      malofta, mola, hele griza 
      <ref tip="super" cel="metal.teralkala0o">teralkala metalo</ref>,
      rimarkinda pro radiado, kies &ccirc;iuj <ref tip="super" cel="izotop.0o">izotopoj</ref> estas
      radioaktivaj:
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> estas blanka, sed nigri&gcirc;os kiam malkovri&gcirc;as al aero, &gcirc;ia
        plej stabila izotopo, Ra-226, havas duonvivon de 1602 jaroj kaj disfalas al
        radona gaso
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=28647">Radiumo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_d;&c_y;&c_j;</trd>
  <trd lng="bg">&c_r;&c_a;&c_d;&c_i;&c_j;</trd>
  <trd lng="br">radiom</trd>
  <trd lng="cs">radium zn. Ra</trd>
  <trd lng="de">Radium</trd>
  <trd lng="en">radium</trd>
  <trd lng="es">radio</trd>
  <trd lng="fr">radium</trd>
  <trd lng="hu">r&aacute;dium</trd>
  <trd lng="id">radium</trd>
  <trd lng="it">radio</trd>
  <trd lng="ja">&#x30E9;&#x30B8;&#x30A6;&#x30E0;</trd>
  <trd lng="la">Radium</trd>
  <trd lng="nl">radium</trd>
  <trd lng="pl">radon</trd>
  <trd lng="pt">r&aacute;dio</trd>
  <trd lng="ru">&c_r;&c_a;&c_d;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>Ra</trd>,
    <trd>r&aacute;dium</trd>
  </trdgrp>
  <trd lng="sv">radium</trd>
  <trd lng="uk">&#x440;&#x430;&#x434;&#x456;&#x439;</trd>
</drv>
</art>
