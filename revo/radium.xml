<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>radium</rad>/o
</kap>
<drv mrk="radium.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 88, atompezo 226,0254, simbolo Ra. Tre malofta metalo,
      rimarkinda pro radiado. &Ccirc;iuj izotopoj de tiu elemento estas
      radioaktivaj.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_d;&c_y;&c_j;</trd>    
  <trd lng="br">radiom</trd>
  <trd lng="de">Radium</trd>   
  <trd lng="fr">radium</trd>
  <trd lng="hu">r&aacute;dium</trd>
  <trd lng="nl">radium</trd>
  <trd lng="ru">&c_r;&c_a;&c_d;&c_i;&c_j;</trd>
</drv>
</art>
