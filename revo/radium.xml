<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>radium</rad>/o
</kap>

<drv mrk="radium.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Ra</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
      lst="voko:&hcirc;emiaj_elementoj" val="88">elemento</ref> kun
      atomnumero 88, atompezo 226,0254; tre 
      malofta, mola, hele griza 
      <ref tip="super" cel="metal.teralkala0o">teralkala metalo</ref>,
      rimarkinda pro radiado, kies &ccirc;iuj <ref tip="super" 
      cel="izotop.0o">izotopoj</ref> estas
      radioaktivaj.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_d;&c_y;&c_j;</trd>
  <trd lng="bg">&c_r;&c_a;&c_d;&c_i;&c_j;</trd>
  <trd lng="br">radiom</trd>
  <trd lng="de">Radium</trd>
  <trd lng="en">radium</trd>
  <trd lng="es">radio</trd> 
  <trd lng="fr">radium</trd>
  <trd lng="hu">r&aacute;dium</trd>
  <trd lng="it">radio</trd>
  <trd lng="nl">radium</trd>
  <trd lng="la">Radium</trd>
  <trd lng="pl">radon</trd>
  <trd lng="pt">r&aacute;dio</trd>
  <trd lng="ru">&c_r;&c_a;&c_d;&c_i;&c_j;</trd>
  <trd lng="sv">radium</trd>
</drv>
</art>
