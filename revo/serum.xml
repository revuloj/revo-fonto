<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: serum.xml,v 1.24 2023/10/21 06:31:44 revo Exp $">
<kap>
  <rad>serum</rad>/o
</kap>
<drv mrk="serum.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Artefarita medikamenta likva&jcirc;o, de variaj kombinoj, por
      subha&ubreve;taj en&scirc;prucigoj:
      <ekz>
        granda kuracisto de la homaro, li <klr>[donis]</klr> ne nur
        konsilojn, anka&ubreve; <tld/>on
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        necesas esti tre singarda, por ne infekti homojn per prionoj
        rezulte de uzado
        de sango, <tld/>o, fermentoj, hormonoj ktp de bovoj &ccirc;e fabrikado
        de medikamentoj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2006/008861.php">Nova etapo en
          biologio kaj medicino</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="ser.0o.serumo">sero<sncref/></ref>
    <ref tip="super" cel="vakcin.0o">vakcino</ref>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_r;&c_o;&c_v;&c_a;&c_t;&c_k;&c_a;</trd>
  <trd lng="ca">s&egrave;rum <klr>(medicament)</klr></trd>
  <trdgrp lng="cs">
    <trd>antis&eacute;rum</trd>,
    <trd>s&eacute;rum</trd>
  </trdgrp>
  <trd lng="de">Immunserum</trd>
  <trd lng="en">serum <klr tip="amb">(medicine)</klr></trd>
  <trd lng="es">suero</trd>
  <trd lng="fa">&a_s;&a_r;&a_m;</trd>
  <trd lng="fr">s&eacute;rum</trd>
  <trd lng="hu">sz&eacute;rum<klr tip="ind">
(k&eacute;sz&iacute;tm&eacute;ny)</klr></trd>
  <trd lng="it">siero<klr tip="amb"> (medicina)</klr></trd>
  <trd lng="ja">&#x8840;&#x6E05;  <pr>&#x3051;&#x3063;&#x305B;&#x3044;</pr></trd>
  <trd lng="nl">serum</trd>
  <trdgrp lng="pl">
    <trd>serum</trd>,
    <trd>surowica  <klr>(lecznicza)</klr></trd>
  </trdgrp>
  <trd lng="pt">s&eacute;rum</trd>
  <trd lng="ru">&c_s;&c_y;&c_v;&c_o;&c_r;&c_o;&c_t;&c_k;&c_a; <klr tip="amb">(&c_p;&c_r;&c_je;&c_p;&c_a;&c_r;&c_a;&c_t;)</klr></trd>
  <trd lng="sk">o&ccaron;kovacia l&aacute;tka</trd>
  <trd lng="sv">serum</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&c_r;&#x43E;&c_v;&c_a;&c_t;&#x43A;&c_a;</trd>,
    <trd>&c_s;&c_je;&c_r;&c_u;&#x43C;</trd>
  </trdgrp>
</drv>
</art>
