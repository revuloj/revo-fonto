<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rak.xml,v 1.11 2024/04/13 20:15:51 revo Exp $">
<kap>
  <rad>rak</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="rak.0o">
  <kap><tld/>o</kap>
  <snc mrk="rak.0o.MEBL">
    <dif>
      Kuna&jcirc;o el stangoj kaj eventuale <ref cel="bret.0o">breto</ref> a&ubreve; hokoj
      por bonorde teni specifajn objektojn (fura&gcirc;on, valizojn, &ccirc;apelojn,
      aparatojn, biciklojn, leterojn k.a.):
      <ekz>
        li trovis la valizon subite tro peza kaj devis peti <klr>[&#x2026;]</klr> pri helpo por
        povi meti &gcirc;in sur la <tld/>o-breton
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        kajuto ekipita per fenestreto, regpanelo, <tld/>o kun elektraj vipiloj
        <fnt>
          <aut>Julia Sigmond, Sen Rodin</aut>
          <vrk><url ref="https://books.google.de/books?id=vTGFDXzWbzgC&amp;lpg=PA221&amp;ots=evIBeTsPcg&amp;pg=PA221#v=onepage"> Libazar' Kaj Tero</url></vrk>
          <lok> Nur kvadrate oni vivas beate, p. 221</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">KOMP</uzo>
        19-cola <tld/>o;
      </ekz>
      <ekz>
        mi momente rigardis liajn valizojn supre en la <tld/>o
        <fnt>
          <aut>W. Elsschot, trad. M. Meeuwissen</aut>,
          <vrk><url ref="http://meeuw.org/fromagxo/node10.html">
          Froma&gcirc;o</url></vrk>,
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bret.0aro">bretaro</ref>,
      <ref cel="fak.0o.ujo">fako<sncref/></ref>,
      <ref cel="hurd.0o.konserva">hurdo<sncref/></ref>,
      <ref cel="korb.0o">korbo</ref>,
      <ref cel="stabl.0o.tabulo">stablo<sncref/></ref>
    </refgrp>
    <bld lok="&WCU;/thumb/6/60/Bicycle_rack_in_Krak&oacute;w%2C_Poland.jpg/640px-Bicycle_rack_in_Krak&oacute;w%2C_Poland.jpg" prm="CC BY-SA 4.0"><tld/>o por bicikloj
        <fnt>
          <aut>Kgbo, propra verko</aut>
          <lok><url ref="&WCW;/File:Bicycle_rack_in_Krak%C3%B3w,_Poland.jpg">https://commons.wikimedia.org/wiki/File:Bicycle_rack_in_Krak&oacute;w,_Poland.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_o;&c_j;&c_k;&c_a; <klr tip="amb">(&c_d;&c_l;&c_ja; &c_r;&c_e;&c_ch;&c_a;&c_w;)</klr></trd>
  <trdgrp lng="cs">
    <trd>jesle</trd>,
    <trd>police</trd>,
    <trd>stojan</trd>,
    <trd>s&iacute;&tcaron; na zavazadla</trd>,
    <trd>v&ecaron;&scaron;&aacute;k</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gestell</trd>,
    <trd>Raufe</trd>,
    <trd>Ablage</trd>,
    <trd>St&auml;nder</trd>,
    <trd>Regal</trd>,
    <trd>Rack</trd>
  </trdgrp>
  <trd lng="en">rack</trd>
  <trdgrp lng="fr">
    <trd>porte-<klr> (support)</klr></trd>,
    <trd>support</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>stojak</trd>,
    <trd>p&oacute;&lstroke;ka</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>poli&tcomma;&abreve;</trd>,
    <trd>raft</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>polica</trd>,
    <trd>sie&tcaron; na bato&zcaron;inu</trd>,
    <trd>stojan</trd>,
    <trd>ve&scaron;iak</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ja;&c_s;&c_l;&c_a; (&c_g;&c_o;&c_d;&c_ib;&c_v;&c_n;&c_i;&c_c;&c_ja; &c_d;&c_l;&c_ja; &c_h;&c_u;&c_d;&c_o;&c_b;&c_i;)</trd>,
    <trd>&c_s;&c_ib;&c_t;&c_k;&c_a; &c_d;&c_l;&c_ja; &c_r;&c_je;&c_ch;&c_je;&c_j; (&c_u; &c_v;&c_a;&c_g;&c_o;&c_n;&c_ib;)</trd>,
    <trd>&c_s;&c_t;&c_ib;&c_j;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_je;&c_l;&c_a;&c_zh;</trd>,
    <trd>&c_k;&c_o;&c_z;&c_l;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_z;&c_l;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x67B6; <pr>ji&#xE0;</pr></trd>
</drv>
</art>

