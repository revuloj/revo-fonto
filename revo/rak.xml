<art mrk="$Id: rak.xml,v 1.8 2022/11/11 09:32:12 revo Exp $">
<kap>
  <rad>rak</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="rak.0o">
  <kap><tld/>o</kap>
  <snc mrk="rak.0o.MEBL">
    <dif>
      Unuopa 
      <ref tip="vid" cel="bret.0o.MEBL">breto</ref>
      a&ubreve; kadro, ofte a&jcirc;ura, kava a&ubreve; iel adaptita
      por la objektoj, kiujn oni sur &gcirc;i ordigas:
      <ekz>
        mi momente rigardis liajn valizojn supre en la <tld/>o
        <fnt>
          <aut>W. Elsschot, trad. M. Meeuwissen</aut>,
          <vrk><url ref="http://meeuw.org/fromagxo/node10.html">
          Froma&gcirc;o</url></vrk>,
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_o;&c_j;&c_k;&c_a; <klr tip="amb">(&c_d;&c_l;&c_ja; &c_r;&c_e;&c_ch;&c_a;&c_w;)</klr></trd>
  <trdgrp lng="cs">
    <trd>jesle</trd>,
    <trd>police</trd>,
    <trd>stojan</trd>,
    <trd>s&iacute;&tcaron; na zavazadla</trd>,
    <trd>v&ecaron;&scaron;&aacute;k</trd>
  </trdgrp>
  <trd lng="en">rack</trd>
  <trdgrp lng="fr">
    <trd>porte-<klr> (support)</klr></trd>,
    <trd>support</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>stojak</trd>,
    <trd>p&oacute;&lstroke;ka</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>poli&tcedil;&abreve;</trd>,
    <trd>raft</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>polica</trd>,
    <trd>sie&tcaron; na bato&zcaron;inu</trd>,
    <trd>stojan</trd>,
    <trd>ve&scaron;iak</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ja;&c_s;&c_l;&c_a; (&c_g;&c_o;&c_d;&c_ib;&c_v;&c_n;&c_i;&c_c;&c_ja; &c_d;&c_l;&c_ja; &c_h;&c_u;&c_d;&c_o;&c_b;&c_i;)</trd>,
    <trd>&c_s;&c_ib;&c_t;&c_k;&c_a; &c_d;&c_l;&c_ja; &c_r;&c_je;&c_ch;&c_je;&c_j; (&c_u; &c_v;&c_a;&c_g;&c_o;&c_n;&c_ib;)</trd>,
    <trd>&c_s;&c_t;&c_ib;&c_j;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_je;&c_l;&c_a;&c_zh;</trd>,
    <trd>&c_k;&c_o;&c_z;&c_l;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_z;&c_l;&c_i;</trd>
  </trdgrp>
</drv>
</art>

