<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>melodi</rad>/o
</kap>
<drv mrk="melodi.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Sinsekvo da muzikaj sonoj agrablaj por la orelo kaj formantaj
      muzikan frazon
    </dif>
    <refgrp tip="sin">
      <ref cel="ari.0o">ario</ref>
    </refgrp>
  </snc>
  <trd lng="de">Melodie</trd>
  <trd lng="en">melody</trd>
  <trdgrp lng="fr">
    <trd>m&eacute;lodie</trd>
  </trdgrp>
  <trd lng="nl">melodie</trd>
  <trdgrp lng="hu">
    <trd>dallam</trd>,
    <trd>mel&oacute;dia</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_je;&c_l;&c_o;&c_d;&c_i;&c_ja;</trd>
  <trd lng="sk">mel&oacute;dia</trd>
</drv>

<drv mrk="melodi.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Konforma al la reguloj de la <tld/>o.
    </dif>
    <trd lng="en">melodic</trd>
    <trdgrp lng="fr">
      <trd>m&eacute;lodique</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>dallam-</trd>,
      <trd>melodikus</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Pro flueco de son-sekvo agrabla por la orelo:
      <ekz>
        la <tld/>a kanto de najtingalo
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="harmon1">harmonia</ref>.
    </refgrp>
    <trdgrp lng="en">
      <trd>melodious</trd>,
      <trd>melodic</trd>,
      <trd>musical</trd>,
      <trd>songful</trd>,
      <trd>tuneful</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>m&eacute;lodieux</trd>
    </trdgrp>
    <trd lng="hu">dallamos</trd>
  </snc>
  <trd lng="de">melodisch</trd>
  <trd lng="nl">melodieus</trd>
  <trd lng="ru">&c_m;&c_je;&c_l;&c_o;&c_d;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">melodick&yacute;</trd>
</drv>
</art>
