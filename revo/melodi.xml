<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>melodi</rad>/o
</kap>
<drv mrk="melodi.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Sinsekvo da muzikaj sonoj agrablaj por la orelo kaj formantaj
      muzikan frazon
    </dif>
    <refgrp tip="sin">
      <ref cel="ari.0o">ario</ref>,
      <ref cel="ritm.0o">ritmo</ref>.
    </refgrp>
  </snc>
  <trd lng="en">melody</trd>
  <trd lng="nl">melodie</trd>
  <trd lng="ru">&c_m;&c_je;&c_l;&c_o;&c_d;&c_i;&c_ja;</trd>
</drv>

<drv mrk="melodi.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Konforma al la reguloj de la <tld/>o.
    </dif>
    <trd lng="en">melodic</trd>
  </snc>
  <snc>
    <dif>
      Belsona, agrabla por la orelo:
      <ekz>
        la <tld/>a kanto de najtingalo
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="harmon">harmonia</ref>.
    </refgrp>
    <trdgrp lng="en">
      <trd>melodious</trd>,
      <trd>melodic</trd>,
      <trd>musical</trd>,
      <trd>songful</trd>,
      <trd>tuneful</trd>
    </trdgrp>
  </snc>
  <trd lng="nl">melodieus</trd>
  <trd lng="ru">&c_m;&c_je;&c_l;&c_o;&c_d;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
