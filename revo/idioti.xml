<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>idiotism</rad>/o
</kap>
<drv mrk="idioti.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">LIN</uzo>
  <snc>
    <dif>
      Nelogika aparta vortokonstruo, propra al iu lingvo sed ne al aliaj:
      <ekz>
        anka&ubreve; en Esperanto trovi&gcirc;as diversaj nemultaj 
        <tld/>oj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        lingvo absolute logika kaj tute sen <tld/>oj estus lingvo senviva
        kaj tro peza<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Idiotismus</trd>,
    <trd>idiomatischer Ausdruck</trd>,
    <trd>Spracheigenheit</trd>,
    <trd>Redewendung</trd>
  </trdgrp>
  <trd lng="en">idiomatic expression</trd>
  <trdgrp lng="fr">
    <trd>idiotisme</trd>,
    <trd>expression idiomatique</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>idiotisme</trd>,
    <trd>zegswijze</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_d;&c_i;&c_o;&c_m;&c_a;</trd>,
    <trd>&c_i;&c_d;&c_i;&c_o;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; &c_v;&c_y;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_f;&c_r;&c_a;&c_z;&c_je;&c_o;&c_l;&c_o;&c_g;&c_i;&c_z;&c_m;</trd>
  </trdgrp>
</drv>
</art>
