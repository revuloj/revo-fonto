<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: merg.xml,v 1.13 2015/07/10 19:10:15 revo Exp $">
<kap>
   <rad>merg</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="merg.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc mrk="merg.0i.TEK">
     <uzo tip="fak">TEK</uzo>
     <dif>
       Meti en likva&jcirc;on, enprofundigi:
       <ekz>
         <tld/>i nitkapon.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_a;&c_k;&c_u;&c_n;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_a;&c_p;&c_u;&c_s;&c_k;&c_a;&c_c;&c_mol;
     <klr tip="amb">
     (&c_u; &c_v;&c_a;&c_d;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;)</klr>
     </trd>,
     <trd>&c_p;&c_a;&c_g;&c_r;&c_u;&c_zh;&c_a;&c_c;&c_mol;
     <klr tip="amb">
     (&c_u; &c_v;&c_a;&c_d;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;)</klr>
     </trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>tauchen</trd>,
     <trd>eintauchen</trd>,
     <trd>untertauchen</trd>
   </trdgrp>
   <trd lng="en">immerse</trd>
   <trd lng="fr">immerger</trd>
   <trdgrp lng="hu">
     <trd>bemer&iacute;t</trd>,
     <trd>lemer&iacute;t</trd>
   </trdgrp>
   <trd lng="nl">onderdompelen</trd>
   <trdgrp lng="pl">
     <trd>zanurza&cacute;</trd>,
     <trd>pogr&aogonek;&zdot;a&cacute;</trd>
   </trdgrp>
   <trdgrp lng="pt">
     <trd>imergir</trd>,
     <trd>mergulhar <klr>(algo em um l&iacute;quido</klr></trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_o;&c_k;&c_u;&c_n;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_o;&c_k;&c_u;&c_n;&c_u;&c_t;&c_mol;</trd>,
     <trd>&c_p;&c_o;&c_g;&c_r;&c_u;&c_z;&c_i;&c_t;&c_mol;
     <klr tip="amb">
     (&c_v; &c_zh;&c_i;&c_d;&c_k;&c_o;&c_s;&c_t;&c_mol;)</klr>
     </trd>
   </trdgrp>
</drv>

<drv mrk="merg.0igxi">
   <kap><tld/>i&gcirc;i</kap>
   <gra><vspec>ntr</vspec></gra>
   <snc mrk="merg.0igxi.TEK">
     <dif>
       Enlikvi&gcirc;i,
       <ref tip="vid" cel="dron.0i">droni</ref>,
       surfundi&gcirc;i:
       <ekz>
         la &scirc;ipo <tld/>i&gcirc;is.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_a;&c_k;&c_u;&c_n;&c_a;&c_c;&c_c;&c_a;</trd>,
     <trd>&c_a;&c_p;&c_u;&c_s;&c_k;&c_a;&c_c;&c_c;&c_a;
     <klr tip="amb">
     (&c_u; &c_v;&c_a;&c_d;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;)</klr>
     </trd>,
     <trd>&c_p;&c_a;&c_g;&c_r;&c_u;&c_zh;&c_a;&c_c;&c_c;&c_a;
     <klr tip="amb">
     (&c_u; &c_v;&c_a;&c_d;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;)</klr>
     </trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>lemer&uuml;l</trd>,
     <trd>elmer&uuml;l</trd>,
     <trd>belemer&uuml;l</trd>
     <trd>els&uuml;llyed</trd>
   </trdgrp>
   <trd lng="nl">zinken</trd>
   <trdgrp lng="pl">
     <trd>pogr&aogonek;&zdot;a&cacute; si&eogonek;</trd>,
     <trd>ton&aogonek;&cacute;</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_o;&c_k;&c_u;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
     <trd>&c_p;&c_o;&c_g;&c_r;&c_u;&c_z;&c_i;&c_t;&c_mol;&c_s;&c_ja;
     <klr tip="amb">
     (&c_v; &c_zh;&c_i;&c_d;&c_k;&c_o;&c_s;&c_t;&c_mol;)</klr>
     </trd>
   </trdgrp>
</drv>

<drv mrk="merg.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc mrk="merg.0igxo.KOMUNE">
    <dif>
      Subi&gcirc;o en likva&jcirc;on.
    </dif>
  </snc>
  <snc mrk="merg.0igxo.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plena eni&gcirc;o en ian medion:
      <ekz>
        kelktagaj lingvaj <tld/>i&gcirc;oj
        <fnt>
          <aut>I. Gon&ccirc;arova</aut>,
          <vrk><url ref="http://sezonoj.ru/2015/07/masi-15/">
          MASI 20-jara. Bela juna a&gcirc;o!</url></vrk>,
          <lok>La Balta Ondo, 2015-07-06</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">immersion</trd>
</drv>

<drv mrk="merg.el0i">
   <kap>el<tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc mrk="merg.el0i.TEK">
     <dif>
       Elmeti el likva&jcirc;o, elprofundigi:
       <ekz>
         el<tld/>i nitkapon.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_v;&c_y;&c_n;&c_u;&c_c;&c_mol;
   <klr tip="amb">
   (&c_z; &c_v;&c_a;&c_d;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_ib;)</klr>
   </trd>
   <trdgrp lng="nl">
     <trd>ophalen</trd>,
     <trd>uittrekken</trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>kidug <klr tip="amb">(folyad&eacute;kb&oacute;l)</klr></trd>,
     <trd>felsz&iacute;nre hoz
     <klr tip="amb">(folyad&eacute;kb&oacute;l)</klr></trd>
   </trdgrp>
   <trd lng="pl">wynurza&cacute;</trd>
   <trd lng="ru">&c_v;&c_y;&c_n;&c_u;&c_t;&c_mol;
   <klr tip="amb">(&c_i;&c_z; &c_zh;&c_i;&c_d;&c_k;&c_o;&c_s;&c_t;&c_i;)
   </klr></trd>
</drv>

<drv mrk="merg.el0igxi">
   <kap>el<tld/>i&gcirc;i</kap>
   <gra><vspec>ntr</vspec></gra>
   <snc mrk="merg.el0igxi.TEK">
     <dif>
       Eli&gcirc;i;
       <ref tip="sin" cel="emergx.0i">emer&gcirc;i</ref>:
       <ekz>
         la roko el<tld/>i&gcirc;as el la akvo.
       </ekz>
     </dif>
   </snc>
   <trd lng="fr">&eacute;merger</trd>
   <trd lng="nl">oprijzen</trd>
   <trd lng="hu">
   felbukkan <klr tip="amb">(folyad&eacute;kb&oacute;l)</klr></trd>
   <trd lng="pl">wynurza&cacute; si&eogonek;</trd>
</drv>

<drv mrk="merg.sub0eblasxipo">
   <kap>sub<tld/>ebla &scirc;ipo</kap>
   <snc>
     <dif>
       &Scirc;ipo povanta veturi surakve kaj enakve:
       <ekz>
         submarboato estas sub<tld/>ebla &scirc;ipo.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_p;&c_a;&c_d;&c_v;&c_o;&c_d;&c_n;&c_a;&c_ja; &c_l;&c_o;&c_d;&c_k;&c_a;</trd>
   <trdgrp lng="de">
     <trd>U-Boot</trd>,
     <trd>Unterseeboot</trd>
   </trdgrp>
   <trd lng="fr">bateau submersible</trd>
   <trd lng="hu">mer&uuml;l&odblac;k&eacute;pes haj&oacute;</trd>
   <trd lng="nl">duikboot</trd>
   <trdgrp lng="pl">
     <trd>&lstroke;&oacute;d&zacute; <ind>podwodna</ind></trd>,
     <trd>okr&eogonek;t <ind>podwodny</ind></trd>
   </trdgrp>
</drv>
</art>
