<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: slab.xml,v 1.9 2023/10/05 08:31:05 revo Exp $">
<kap>
  <rad>slab</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="slab.0o">
  <kap><tld/>o</kap>
  <snc mrk="slab.0o.KON">
    <uzo tip="fak">KON</uzo>
    <dif>
      Granda <ref tip="super" cel="plat.0o.KOMUNE">plato</ref> el
      &scirc;tono, betono a&ubreve; alia solida, peza substanco, uzebla en
      konstrulaboroj:
      <ekz>
        vi trovos en tiu kelo <tld/>on de la <ref cel="pavim.0o">pavimo</ref>, kiun oni delokis
        <fnt>
        <aut>V. Hugo, trad. D. Luez</aut>,
        <vrk>Angelo, Tirano de Padovo, 3:2:11</vrk>,
        <lok>Franca Esperantisto, 1986-07, numero 373a</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u iu subita frapo per &scirc;ia mano forfrapis la subtenon kaj igis
        la <tld/>on fali
        <fnt><aut>A. Conan Doyle, trad D. Booton</aut>, <vrk><url
        ref="http://members.tripod.com/~booton/musgravo/">La musgrava
        rito</url></vrk>, <lok>[vidita en 2006-12]</lok></fnt>?
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="plat.sxton0o">&scirc;tonplato</ref>,
       <ref cel="tabul.0o">tabulo</ref>,
       <ref cel="tegol.0o">tegolo</ref>,
       <ref cel="sxind.0o">&scirc;indo</ref>,
       <ref cel="kahel.0o">kahelo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ib;&c_t;&c_a; <klr tip="amb">(&c_k;&c_a;&c_n;&c_s;&c_t;&c_r;&c_u;&c_k;&c_c;&c_y;&c_j;&c_n;&c_a;&c_ja;, &c_b;&c_u;&c_d;&c_a;&c_w;&c_n;&c_ib;&c_ch;&c_a;&c_ja;)</klr></trd>
  <trd lng="cs">deska</trd>
  <trd lng="de">Deckplatte</trd>
  <trd lng="en">slab</trd>
  <trd lng="fr">dalle</trd>
  <trd lng="it">lastra</trd>
  <trd lng="nl">dekplaat</trd>
  <trdgrp lng="pl">
    <trd>p&lstroke;yta</trd>,
    <trd>tafla</trd>
  </trdgrp>
  <trd lng="sk">doska</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43B;&#x44F;&c_b;</trd>,
    <trd>&#x43F;&#x43B;&c_i;&c_t;&c_a;</trd>,
    <trd>&#x43F;&#x43B;&c_a;&c_s;&c_t;&c_i;&#x43D;&c_a;</trd>
  </trdgrp>
</drv>
</art>

