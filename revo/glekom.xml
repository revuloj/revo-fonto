<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: glekom.xml,v 1.19 2023/11/11 11:36:35 revo Exp $">
<kap>
<rad>glekom</rad>/o
</kap>
<drv mrk="glekom.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el la familio <ref tip="malprt" cel="lamiac.0oj">lamiacoj</ref>
      (<trd lng="la">Glechoma</trd>);
      ku&scirc;anta <ref tip="lst" cel="herb.0o.BOT" lst="voko:lamioparencoj">herbo</ref>
      kun kor- a&ubreve; renformaj dentrandaj a&ubreve; no&ccirc;itaj folioj;
      kun blue violkoloraj labiaj floroj, kies triloba malsupra lipo
      havas
      grandan mezan lobon kun <ctl>barbeto</ctl>;
      kun du mallongaj kaj du pli longaj stamenoj:
      <ekz>
        glekomo anka&ubreve; estas uzata kiel kuracherbo kontra&ubreve; jukado kaj por vundoj,
        <klr>[&#x2026;]</klr> helpas kontra&ubreve; juko kiun oni ekhavas de urtikoj, en kies
        najbareco &gcirc;i ofte kreskas <klr>[kaj en]</klr> en bierfarejoj,
        <klr>[kie]</klr> &gcirc;i estis la anta&ubreve;ulo de lupolo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=257384">Glekomo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/glekom.jpg">
    <ind>rampanta <tld/>o</ind>
    <trd lng="ca"><ind>heura</ind> de terra</trd>
    <trdgrp lng="fr">
        <trd>Lierre terresre</trd>,
        <trd>Herbe de la St-Jean</trd>,
        <trd>Courroie de la St-Jean</trd>,
        <trd>Drienne</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kerek <ind>repk&eacute;ny</ind></trd>,
      <trd>f&ouml;ldi <ind>repk&eacute;ny</ind></trd>,
      <trd>f&ouml;ldiborosty&aacute;n</trd>
    </trdgrp>
    <trd lng="la">Glechoma hederacea</trd>
    <trd lng="nl">hondsdraf</trd>
  </bld>
  <trdgrp lng="be">
    <trd>&c_b;&c_l;&c_ju;&c_sh;&c_ch;&c_y;&c_k;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_h;&c_o;&c_d;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">g&egrave;nere Glechoma</trd>
  <trd lng="de">Gundermann</trd>
  <trd lng="hu">repk&eacute;ny</trd>
  <trd lng="ja">&#x30AB;&#x30AD;&#x30C9;&#x30AA;&#x30B7;</trd>
  <trd lng="nl">glechoma</trd>
  <trd lng="ru">&c_b;&c_u;&c_d;&c_r;&c_a;</trd>
  <trd lng="sv">Jordreva</trd>
  <trd lng="uk">&#x440;&#x43E;&#x437;&#x445;&#x456;&#x434;&#x43D;&#x438;&#x43A;</trd>
  <trd lng="zh">&#x91D1;&#x9322;&#x8584;&#x8377; <pr>j&#x12B;nqi&#xE1;nb&#xF2;he</pr></trd>
</drv>
</art>
