<drv mrk="glekom.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super"
      cel="lamiac.0oj">lamiacoj</ref>
      (<trd lng="la">Glechoma</trd>).
