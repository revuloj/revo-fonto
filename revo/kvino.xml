<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: kvino.xml,v 1.11 2024/03/10 21:19:59 revo Exp $">
  <kap>
    <rad>kvino</rad>/o  <fnt><bib>PIV2</bib></fnt>,
    <var><kap><rad var="u">kinu</rad>/o <fnt><aut>Fernando de Diego</aut> <vrk><url
    ref="http://www.esperanto.es:8080/diccionario/inicio.jsp">Gran Diccionario 
    Espa&ntilde;ol-Esperanto</url></vrk></fnt></kap></var>
  </kap>

  <drv mrk="kvino.0o">
    <kap><tld/>o,
      <var><kap>kinuo</kap></var>
    </kap>
    <snc mrk="kvino.0o.BOT">
      <uzo tip="fak">AGR</uzo>
      <uzo tip="fak">BOT</uzo>
      <dif>
        <ref tip="super" cel="cereal.0o.AGR">Cerealo</ref> el
        Sudameriko,
        kun etaj, iom amargustaj grajnoj, kultivebla &gcirc;is alte en
        montaroj, specio el la genro
        <ref tip="super" cel="kenopodi.0o.BOT">kenopodio</ref>
        (<trd lng="la">Chenopodium quinoa</trd>):
        <ekz>
          ekzistas ne-graminacaj plantoj, kiujn oni simile kiel grenon
          kultivas pro iliaj semoj, ekzemple <tld/>o
          <fnt>
            <bib>Viki</bib>,
            <lok>greno, 2005-08-14</lok>
          </fnt>;
        </ekz>
        <ekz>
          en la unua rondo de gustumado estis ofertita <tld var="u"/>o
          en la spica versio, kiu estis volonte kaj la&ubreve;de akceptata
          <fnt>
            <aut>Roland Schnell</aut>:
            <vrk><url ref="http://www.liberafolio.org/Members/Agrokarbo">Malfermo de kinuo-jaro 2013
            en Berlin</url></vrk>.
            <lok>Libera Folio</lok>
          </fnt>.
        </ekz>
      </dif>
    </snc>
    <trd lng="de">Quinoa</trd>
    <trd lng="en">quinoa</trd>
    <trd lng="el">&kappa;&jota;&ny;&omikron_ton;&alfa;</trd>
    <trd lng="fr">quinoa</trd>
    <trdgrp lng="ja">
      <trd>&#x30AD;&#x30CC;&#x30A2;</trd>,
      <trd>&#x30AD;&#x30CE;&#x30A2;</trd>
    </trdgrp>
    <trd lng="nl">quinoa</trd>
    <trdgrp lng="pl">
      <trd>quinoa</trd>,
      <trd>ry&zdot; peruwia&nacute;ski</trd>,
      <trd>ziarno komosy ry&zdot;owej</trd>
    </trdgrp>
    <trd lng="pt">quinoa</trd>
    <trd lng="ru">&c_k;&c_i;&c_n;&c_o;&c_a;</trd>
    <trdgrp lng="zh">
    <trd>&#x85DC;&#x9EA5; <pr>l&#xED;m&#xE0;i</pr></trd>,
    <trd>&#x85DC;&#x9EA6; <pr>l&#xED;m&#xE0;i</pr></trd>
  </trdgrp>
</drv>
</art>

