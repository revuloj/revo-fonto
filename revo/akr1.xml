<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: akr1.xml,v 1.5 2023/08/30 18:23:31 afido Exp $">
    <kap>
      <rad>Akr</rad>/o
    </kap>

<drv mrk="akr1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      La <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&ccirc;efurbo</ref>
      de <ref tip="malprt" cel="gana.G0o">Ganao</ref>:
      <ekz>
        finfine, mi povis voja&gcirc;i al <tld/>o, la &ccirc;ef-urbo de Ganao
        <fnt>
          <vrk><url ref="https://ttt.esperanto.it/kirek/download/tekstoj/kontakto_5_2005_pagxo12.pdf">Sep jarojn post disi&gcirc;o</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        tabomoj estas anoj de la afrik-brazila komunumo en <tld/>o, la &ccirc;efurbo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=238425">Tabomoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_A;&c_k;&c_r;&c_a;</trd>
    <trd lng="de">Accra</trd>
    <trd lng="en">Accra</trd>
    <trd lng="fr">Accra</trd>
  </snc>
  <rim>
    Mezuro de surfaco anka&ubreve; nomi&gcirc;as <ref tip="hom" cel="akre.0o">akro (akreo)</ref>.
  </rim>
  <trd lng="ja">&#x30A2;&#x30AF;&#x30E9;</trd>
</drv>

  </art>

