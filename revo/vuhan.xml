<vortaro>
  <art mrk="$Id: vuhan.xml,v 1.6 2023/09/05 09:43:57 afido Exp $">
    <kap>
      <rad>Vuhan</rad>/o
    </kap>

<drv mrk="vuhan.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Granda <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref> 
      en la oriento de <ref tip="malprt" cel="cxin.CX0ujo">&Ccirc;inujo</ref>
      &ccirc;e la kunfluejo de <ref cel="jangzi.0o">Jangzio</ref> 
      kaj Han-Rivero:
      <ekz>
        somero en <tld/>o estas pli varma ol en multaj aliaj urboj de &Ccirc;inio
        <fnt>
          <vrk><url ref="&Viki;/Vuhano">Vuhano - Vikipedio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en la fino de la jaro 2019 eksonis la unuaj informoj pri la nova kronviruso, en
        la provinco <tld/>o
        <fnt>
          <vrk><url ref="&Sezonoj;2020/07/pandemio-3/">Kronviruso en Brazilo - La Ondo de
          EsperantoLa Ondo de ...</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_U;&c_h;&c_a;&c_n;&c_mol;</trd>
    <trd lng="de">Wuhan</trd>
    <trd lng="en">Wuhan</trd>
    <trd lng="fr">Wuhan</trd>
    <trd lng="ru">&c_U;&c_h;&c_a;&c_n;&c_mol;</trd>
    <trd lng="zh">&#x6B66;&#x6C49; <klr>(w&#x1D4;h&agrave;n)</klr></trd>
  </snc>
  <trd lng="ja">&#x30A6;&#x30FC;&#x30CF;&#x30F3;</trd>
  <trd lng="uk">&#x423;&#x445;&#x430;&#x43D;&#x44C;</trd>
</drv>

  </art>

