<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: eglanteri.xml,v 1.11 2020/03/14 10:59:08 revo Exp $">
<kap>
  <rad>eglanteri</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="eglanteri.0o">
  <kap><tld/>o</kap>
  <snc mrk="eglanteri.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Sova&gcirc;a <ref tip="vid" cel="roz.0o.ARBO">rozo</ref>, dorna arbeto
      kun
      belaj floroj kreskanta en mezvarmaj landoj de E&ubreve;razio,
      <ref tip="lst" cel="speci.0o.BIO"
      lst="voko:botanikaj_specioj">specio</ref> de <ref tip="super"
      cel="roz.0arbo">rozarbo</ref>
      (<trd lng="la">Rosa canina</trd>):
      <ekz>
        dol&ccirc;a kaj ekscita aromo de <tld/>oj
        <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 22a, p. 127a</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;irka&ubreve;e abundege kreskis <tld/>oj kun bongustaj beroj
        <fnt>
        <aut>-</aut>,
        <vrk>OkSEJT: kio venos?</vrk>,
        <bib>LOdE</bib>,
        <lok><url
        ref="http://esperanto-ondo.ru/72-lode.htm">2000:10</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>gavarrera</trd>,
    <trd><ind>roser</ind> can&iacute;</trd>,
    <trd><ind>roser</ind> silvestre</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>plan&aacute; r&uring;&zcaron;e</trd>,
    <trd>&scaron;&iacute;pek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hundsrose</trd>,
    <trd>Heckenrose</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;glantier</trd>,
    <trd>&eacute;glantine</trd>
  </trdgrp>
  <trd lng="pl">r&oacute;&zdot;a dzika</trd>
  <trd lng="pt">rosa-canina</trd>
  <trd lng="ru">&c_sh;&c_i;&c_p;&c_o;&c_v;&c_n;&c_i;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>plan&aacute; ru&zcaron;a</trd>,
    <trd>&scaron;&iacute;pka</trd>
  </trdgrp>
</drv>
</art>

