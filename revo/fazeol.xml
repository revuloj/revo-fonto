    <trd>fasola szparagowa</trd>,
    <trd>fasolka szparagowa</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_a;&c_s;&c_o;&c_l;&c_mol;</trd>
  <trd lng="sv">b&ouml;na</trd>
  <trd lng="tr">fasulye</trd>
</drv>
