<vortaro>
<art mrk="$Id$">
<kap>
<rad>genez</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="genez.0o">
  <kap><tld/>o</kap>
  <snc mrk="genez.0o.BIB">
    <uzo tip="fak">BIB</uzo>
    <uzo tip="fak">REL</uzo>
    <dif>
      La unua libro de <ref tip="malprt" cel="bibli.0o">Biblio</ref>,
      kies komenco temas pri la kreo de la mondo.
    </dif>
    <trd lng="de">Genesis</trd>
    <trd lng="es">G&eacute;nesis</trd>
    <trd lng="fr">Gen&egrave;se</trd>
    <trdgrp lng="hu">
      <trd>Genezis</trd>,
      <trd>Teremt&eacute;s k&ouml;nyve</trd></trdgrp>
    <trd lng="nl">Genesis</trd><trd lng="ru">&c_k;&c_n;&c_i;&c_g;&c_a; <ind>&c_B;&c_y;&c_t;&c_i;&c_ja;</ind></trd>
  </snc>
  <snc mrk="genez.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Esti&gcirc;o, origino, deveno:
      <ekz>
        skizi la <tld/>on de la ideo pri lingvo internacia;
      </ekz>
      <ekz>
        <ref cel="etn.0ogenezo">etno<tld/>o</ref>;
      </ekz>
      <ekz>
        <ref cel="embrio.0o">embrio<tld/>o</ref>.
      </ekz>
    </dif>
    <trd lng="de">Genese</trd>
    <trd lng="es">g&eacute;nesis</trd>
    <trd lng="fr">gen&egrave;se</trd>
    <trd lng="hu">genezis</trd>
    <trd lng="nl">genese</trd>
    <trd lng="ru">&c_g;&c_je;&c_n;&c_je;&c_z;&c_i;&c_s;</trd>
  </snc>
  <adm>
  Uskleco? <aut>MB</aut>
  </adm>
</drv>
</art>
