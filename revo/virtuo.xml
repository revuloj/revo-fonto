<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: virtuo.xml,v 1.18 2018/08/15 18:10:14 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>virtuoz</rad>/o
</kap>
<drv mrk="virtuo.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="talent.0ulo">Talentulo</ref>:
      <ekz>
        vi anka&ubreve; estas <tld/>o &dash; vi ludas fluton, kaj vi mem &gcirc;in eltran&ccirc;as
        <fnt>
          <bib>Fab3</bib>
          <lok>&Ccirc;io sur sian &gcirc;ustan lokon!</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Diesel</nom> <klr>[&#8230;]</klr> anka&ubreve; estis lingvosciencisto kaj <tld/>o de artoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3678">Rudolf Diesel</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_r;&c_t;&c_u;&c_o;&c_z;</trd>
  <trd lng="ca">virtu&oacute;s</trd>
  <trd lng="cs">virtu&oacute;z</trd>
  <trdgrp lng="de">
    <trd>Virtuose</trd>,
    <trd>Genie</trd>
  </trdgrp>
  <trd lng="en">virtuoso</trd>
  <trd lng="es">virtuoso<klr> (en la m&uacute;sica)</klr></trd>
  <trdgrp lng="fr">
    <trd>g&eacute;nie</trd>,
    <trd>virtuose</trd>
  </trdgrp>
  <trd lng="hu">virtu&oacute;z</trd>
  <trd lng="it">virtuoso<klr> (musica)</klr></trd>
  <trd lng="nl">virtuoos</trd>
  <trd lng="pt">virtuoso</trd>
  <trd lng="ru">&c_v;&c_i;&c_r;&c_t;&c_u;&c_o;&c_z;</trd>
  <trdgrp lng="sk">
    <trd>majster</trd>,
    <trd>virtu&oacute;z</trd>
  </trdgrp>
</drv>
</art>
