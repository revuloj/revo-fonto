<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>elokvent</rad>/a
</kap>
<drv mrk="elokve.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc num="1">
    <dif>
      Posedanta la talenton facile, konvinke k bele paroladi:
      <ekz>
        <tld/>a oratoro;
      </ekz>
      <ekz>
        la <ind><tld/>eco</ind> <trd lng="ru">&c_k;&c_r;&c_a;&c_s;&c_n;&c_o;&c_r;&c_je;&c_ch;&c_i;&c_je;</trd>
        estas anima kapablo, kiu igas nin estroj super la koro
        k spirito de aliaj;
      </ekz>
      <ekz>
        la tro konstanta <tld/>eco enuigas;
      </ekz>
      <ekz>
        sciu <tld/>i, sciu anka&ubreve; silenti
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>.
      </ekz> 
    </dif>
    <trd lng="ru">&c_k;&c_r;&c_a;&c_s;&c_n;&c_o;&c_r;&c_je;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>
  </snc>
  <snc num="2">
    <dif>
      Bela k konvinka:
      <ekz>
        <tld/>a parolado, stilo, rigardo.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_a;&c_s;&c_n;&c_o;&c_r;&c_je;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>,
      <trd>&c_v;&c_y;&c_r;&c_a;&c_z;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="de">
    <trd>redegewandt</trd>,
    <trd>beredt</trd>
  </trdgrp>
</drv>
</art>
