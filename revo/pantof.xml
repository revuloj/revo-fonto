<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pantof.xml,v 1.27 2024/01/22 12:43:29 revo Exp $">
<kap>
  <ofc>*</ofc><rad>pantofl</rad>/o
</kap>
<drv mrk="pantof.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Hejma <ref tip="super" cel="vest.pied0o">piedvesto</ref>, malpeza
      kaj oportuna, ne atinganta la maleolojn:
      <ekz>
        li kuris tiel rapide, ke unu <tld/>o desaltis
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        maljuna re&gcirc;o, en nokta robo kaj broditaj <tld/>oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        li metis la piedojn en karese lozajn <tld/>ojn el blua &scirc;amo
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 1</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="babusx.0o">babu&scirc;o</ref>,
      <ref cel="sandal.0o">sandalo</ref>,
      <ref cel="zori.0o">zorio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_n;&c_t;&c_o;&c_f;&c_l;&c_ja;</trd>
  <trdgrp lng="br">
    <trd>chaoson</trd>,
    <trd>kofignon</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>plantofa</trd>,
    <trd>sabatilla</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pantofel</trd>,
    <trd>papu&ccaron;e</trd>,
    <trd>trepka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hausschuh</trd>,
    <trd>Hausschlappen</trd>,
    <trd>Pantoffel</trd>,
    <trd>Schlappen</trd>
  </trdgrp>
  <trd lng="en">slipper</trd>
  <trd lng="es">pantufla</trd>
  <trdgrp lng="fr">
    <trd>chausson<klr tip="ind"> (pantoufle)</klr></trd>,
    <trd>pantoufle</trd>
  </trdgrp>
  <trd lng="hu">papucs</trd>
  <trdgrp lng="it">
    <trd>ciabatta</trd>,
    <trd>pantofola</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30B9;&#x30EA;&#x30C3;&#x30D1;</trd>,
    <trd>&#x5BA4;&#20869;&#12400;&#x304D;  <pr>&#12375;&#12388;&#x306A;&#12356;&#12400;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="nl">pantoffel</trd>
  <trdgrp lng="pl">
    <trd>pantofel</trd>,
    <trd>pape&cacute;</trd>,
    <trd>kape&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>pantufo</trd>,
    <trd>pantufa</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_a;&c_p;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_a;&c_p;&c_o;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pantof&lcaron;a</trd>,
    <trd>papu&ccaron;a</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_a;&c_n;&c_t;&c_o;&c_f;&c_l;&c_ja;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_t;&c_o;&c_f;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_k;&c_a;&c_p;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="pantof.0i">
  <kap>
    <tld/>i
  </kap>
  <snc mrk="pantof.0i.tiriri">
    <dif>
      <ref tip="super" cel="ir.0i.deAl">Iri</ref> en <tld/>oj,
      ordinare apena&ubreve; levante la piedojn:
      <ekz>
        tire <tld/>is la lacaj pa&scirc;oj
        <fnt>
          <bib>LGA</bib>
          <lok>I.</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>is kuirejen
        <fnt>
          <bib>ChC</bib>
          <lok>3</lok>
        </fnt>;
      </ekz>
      <ekz>
        la a&gcirc;ulino elliti&gcirc;is kaj &ccirc;agrene <tld/>is al la komodo
        <fnt>
          <bib>VRA</bib>
          <lok>Jan Frideg&aring;rd</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="pland.0i">plandi, plandumi</ref>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_o;&c_ch;&c_y;&c_c;&c_mol; &c_u; <ind>&c_p;&c_a;&c_n;&c_t;&c_o;&c_f;&c_l;&c_ja;&c_h;</ind></trd>
  <trd lng="de">schlurfen <klr>(in Pantoffeln)</klr></trd>
</drv>

</art>
