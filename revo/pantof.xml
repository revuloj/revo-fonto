<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc><rad>pantofl</rad>/o</kap>
<drv mrk="pantof.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Hejma <ref tip="super" cel="vest.pied0o">piedvesto</ref>, malpeza kaj
      oportuna, ne atinganta la maleolojn.
    </dif>
    <refgrp tip="vid">
      <ref cel="sandal.0o">sandalo</ref>,
      <ref cel="babusx.0o">babu&scirc;o</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Pantoffel</trd>
  <trdgrp lng="fr">
    <trd>chausson<klr tip="ind"> (pantoufle)</klr></trd>,
    <trd>pantoufle</trd>
  </trdgrp>
  <trd lng="nl">pantoffel</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_a;&c_p;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_a;&c_p;&c_o;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
