<art mrk="$Id: pantof.xml,v 1.23 2023/01/03 20:29:40 revo Exp $">
<kap>
  <ofc>*</ofc><rad>pantofl</rad>/o
</kap>
<drv mrk="pantof.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Hejma <ref tip="super" cel="vest.pied0o">piedvesto</ref>, malpeza
      kaj
      oportuna, ne atinganta la maleolojn:
      <ekz>post tio li metis la piedojn en <tld/>ojn
      <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 53a, p.
188a</lok></fnt>.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="babusx.0o">babu&scirc;o</ref>,
      <ref cel="sandal.0o">sandalo</ref>..
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_n;&c_t;&c_o;&c_f;&c_l;&c_ja;</trd>
  <trdgrp lng="br">
    <trd>chaoson</trd>,
    <trd>kofignon</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>plantofa</trd>,
    <trd>sabatilla</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pantofel</trd>,
    <trd>papu&#x10D;e</trd>,
    <trd>trepka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hausschuh</trd>,
    <trd>Hausschlappen</trd>,
    <trd>Pantoffel</trd>,
    <trd>Schlappen</trd>
  </trdgrp>
  <trd lng="en">slipper</trd>
  <trd lng="es">pantufla</trd>
  <trdgrp lng="fr">
    <trd>chausson<klr tip="ind"> (pantoufle)</klr></trd>,
    <trd>pantoufle</trd>
  </trdgrp>
  <trd lng="hu">papucs</trd>
  <trdgrp lng="it">
    <trd>ciabatta</trd>,
    <trd>pantofola</trd>
  </trdgrp>
  <trd lng="nl">pantoffel</trd>
  <trdgrp lng="pl">
    <trd>pantofel</trd>,
    <trd>pape&cacute;</trd>,
    <trd>kape&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>pantufo</trd>,
    <trd>pantufa</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_a;&c_p;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_a;&c_p;&c_o;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pantof&#x13E;a</trd>,
    <trd>papu&#x10D;a</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_a;&#x43D;&c_t;&#x43E;&c_f;&#x43B;&#x44F;</trd>,
    <trd>&#x43F;&c_a;&#x43D;&c_t;&#x43E;&c_f;&c_je;&#x43B;&#x44C;</trd>,
    <trd>&#x43A;&c_a;&#x43F;&c_je;&c_c;&#x44C;</trd>
  </trdgrp>
</drv>
</art>
