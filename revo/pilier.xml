<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pilier.xml,v 1.15 2024/05/10 09:24:57 revo Exp $">
<kap>
  <rad>pilier</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="pilier.0o">
  <kap><tld/>o</kap>
  <snc mrk="pilier.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Masonita <ref tip="super" cel="kolon.0o.ARKI">kolon</ref>ego
      subtenanta grandan konstrua&jcirc;on:
      <ekz>
        muroj, plafonoj kaj <tld/>oj estas ornamitaj de koloritaj
        freskoj ilustrantaj la historion de Buddo
        <fnt>
          <aut>Phan Hong Vuong</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/78092/132652">
          La Pagodo Monivongsa Bopharam</url></vrk>,
          <lok>2009-02-28</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pilast.0o">pilastro</ref>,
      <ref cel="pilon.0o">pilono</ref>
    </refgrp>
  </snc>
  <snc mrk="pilier.0o.FIG">
     <uzo tip="stl">FIG</uzo>
     <dif>
     Io a&ubreve; iu sur kiu io alia bazi&gcirc;as:
     <ekz>
     la kvar <tld/>oj, sur kiuj bazi&gcirc;as nia nuna civilizacio: amoniako, plasto, &scirc;talo kaj cemento
     <fnt>
        <aut>Roland Rotsaert</aut>
        <vrk><url ref="&Monato;/2023/013303.php">Kiel la mondo reale funkcias</url></vrk>
        <bib>Monato</bib>
        <lok>2023/05, p. 19</lok>
        </fnt>;
     </ekz>
     </dif>
     </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_l;&c_u;&c_p;</trd>,
    <trd>&c_a;&c_p;&c_o;&c_r;&c_a;</trd>,
    <trd>&c_b;&c_y;&c_k; <klr tip="amb">(&c_m;&c_a;&c_s;&c_t;&c_a;)</klr></trd>,
    <trd>&c_u;&c_s;&c_t;&c_o;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>pilastra</trd>,
    <trd>pilastr&oacute;</trd>
  </trdgrp>
  <trd lng="cs">pil&iacute;&rcaron; <klr>(mostn&iacute;)</klr></trd>
  <trd lng="de"><klr>(Br&uuml;cken-)</klr>Pfeiler</trd>
  <trd lng="en">pillar</trd>
  <trd lng="fr">pilier</trd>
  <trdgrp lng="ja">
    <trd>&#x6A4B;&#x811A;  <pr>&#x304D;&#12423;&#12358;&#x304D;&#12419;&#x304F;</pr></trd>,
    <trd>&#x305B;&#x308A;&#x53F0;  <pr>&#x305B;&#x308A;&#12384;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">pijler</trd>
  <trd lng="pl">filar mostowy</trd>
  <trd lng="ro">st&acirc;lp de pod</trd>
  <trd lng="sk">pilier <klr>(mostn&yacute;)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_ib;&c_d;&c_v;&c_a;&#x43B;&c_i;&#x43D;&c_a;</trd>,
    <trd>&c_s;&c_t;&#x43E;&#x44F;&#x43A;</trd>,
    <trd>&c_s;&c_t;&#x43E;&c_v;&#x43F;</trd>,
    <trd>&#x43E;&#x43F;&#x43E;&c_r;&c_a;</trd>,
    <trd>&c_b;&c_i;&#x43A; (&#x43C;&#x43E;&c_s;&c_t;&c_a;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x652F;&#x67F1; <pr>zh&#x12B;zh&#xF9;</pr></trd>,
    <trd>&#x67F1;&#x5B50; <pr>zh&#xF9;zi</pr></trd>
  </trdgrp>
</drv>
</art>

