<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: lavab.xml,v 1.3 2024/01/12 21:23:19 afido Exp $">
    <kap>
      <rad>lavab</rad>/o <fnt><bib>PIV</bib></fnt>
    </kap>

<drv mrk="lavab.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
       <ref tip="super" cel="pelv.lavo0o">Lavopelvo</ref> kun krano(j) kaj defluilo:
       <ekz>
          lo&gcirc;ejeto, kun anta&ubreve;&ccirc;ambro, kaj en &gcirc;i <tld/>o
        <fnt>
          <bib>Sezonoj</bib>
          <aut>M. A. Bulgakov</aut>
          <vrk>La majstro kaj Margarita</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;ambroj por 1-4 personoj, havas komfortajn litojn, <tld/>on, bideon, senpordan
        vest&scirc;rankon
        <fnt>
          <bib>LOdE</bib>
          <aut>Domo Grezijono</aut>
          <vrk>Kultura Esperanto</vrk>
          <lok>Sendependa &Ccirc;iumonata Revuo, 2003. n-ro 12 (110)</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="lav.0ujo">lavujo</ref>
    <ref tip="vid" cel="vask1.0o">vasko</ref>
    <trdgrp lng="ar">
      <trd>&a_A;&a_l;&a_H;&a_w;&a_D;</trd>
    </trdgrp>
    <trdgrp lng="cs">
      <trd>lavabo</trd>,
      <trd>umyvadlo</trd>
    </trdgrp>
    <trdgrp lng="da">
      <trd>lavabo</trd>,
      <trd>h&aring;ndvask</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Waschbecken</trd>,
      <trd>Waschtisch</trd>,
      <trd>Bassena</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>lavabo</trd>,
      <trd>sink</trd>
    </trdgrp>
    <trd lng="es">lavabo</trd>
    <trd lng="fr">lavabo</trd>
    <trd lng="hr">umivaonik</trd>
    <trd lng="id">wastafel</trd>
    <trd lng="it">lavabo</trd>
    <trd lng="ka">&#4305;&#4304;&#4313;&#4304;&#4316;&#4312;</trd>
    <trdgrp lng="nl">
      <trd>wastafel</trd>,
      <trd>lavabo</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>lawaterz</trd>,
      <trd>umywalka</trd>
    </trdgrp>
    <trd lng="ro">lavabou</trd>
    <trd lng="ru">&c_r;&c_u;&c_k;&c_o;&c_m;&c_o;&c_j;&c_n;&c_i;&c_k;</trd>
    <trd lng="sk">um&yacute;vadlo</trd>
    <trd lng="uk">&c_r;&c_u;&c_k;&c_o;&c_m;&c_i;&c_j;&c_n;&c_i;&c_k;</trd>
  </snc>
<adm>
  la tradukojn necesas kontroli.
</adm>
</drv>

  </art>

