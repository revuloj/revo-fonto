<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: razi.xml,v 1.5 2024/04/03 20:24:23 revo Exp $">
<kap>
  <rad>razi</rad>/i
</kap>
<drv mrk="razi.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="razi.0i.MIL">
    <dif>
      Surprizataki por <ref tip="super" cel="rab.0i">rabado</ref> a&ubreve; forkaptado.
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_p;&c_a;&c_d;&c_a;&c_c;&c_mol; <klr>(&c_z; &c_m;&c_e;&c_t;&c_a;&c_j; &c_r;&c_a;&c_b;&c_u;&c_n;&c_k;&c_u;)</klr></trd>,
      <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_n;&c_a;&c_b;&c_je;&c_g;</ind></trd>
    </trdgrp>
    <trd lng="de">auf <ind>Beutezug</ind> gehen</trd>
    <trd lng="fr">razzier</trd>
  </snc>
  <snc mrk="razi.0i.POL">
    <dif>
      Surprize kaj amplekse <ref tip="super" cel="kontro.tra0i">trakontroli</ref> lokon, ofte kun arestadoj:
      <ekz>
        je la 6a horo, polico en Britio <tld/>is la domon
        <fnt>
          <aut> Pa&ubreve;lo Gubbins </aut>
          <vrk><url ref="https://archive.org/details/InternaciaJhurnalisto_1992_n4"
              >Dan&gcirc;era Farso</url></vrk>
          <lok> Internacia &Jcirc;urnalisto, n-ro 4/1992, p.6</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_a;&c_b;&c_l;&c_a;&c_v;&c_u;</ind> <klr>(&c_p;&c_a;&c_l;&c_ib;&c_c;&c_e;&c_j;&c_s;&c_k;&c_u;&c_ju;)</klr></trd>
    <trd lng="de">eine <ind>Razzia</ind> durchf&uuml;hren</trd>
    <trd lng="fr">rafler<klr> (police etc.)</klr></trd>
    <trd lng="pl">dokonywa&cacute; nalotu policyjnego</trd>
    <trd lng="ro">face un raid al poli&tcomma;iei </trd>
  </snc>
</drv>

<drv mrk="razi.0o">
  <kap><tld/>o  <fnt><bib>PIV1</bib></fnt></kap>
  <snc mrk="razi.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Rab<ref tip="super" cel="atak.0o.batalo">atako</ref>:
      <ekz>
        judoj postklarigis <klr>[&#8230;]</klr> la venkojn kaj <tld/>ojn super aliaj gentoj
        kiel alahajn favorojn a&ubreve; rekompencojn
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk>Plia libro biblia</vrk>
        </fnt>;
      </ekz>
      <ekz>
        post la fino de la <tld/>o <klr>[&#8230;]</klr>
        &ccirc;iun indi&gcirc;enon pli ol kvardek-jaran oni senkompate bu&ccirc;as
        <fnt>
          <aut>A. Montagut</aut>,
          <vrk><url ref="http://www.liberafolio.org/2009/vernesperanto3">
          Verne patro kaj Verne filo</url></vrk>,
          <lok>libera folio, 2009-02-17</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_n;&c_a;&c_b;&c_je;&c_g;</trd>
    <trdgrp lng="de">
      <trd>Streifzug</trd>,
      <trd>Beutezug</trd>
    </trdgrp>
    <trd lng="fr">razzia</trd>
  </snc>
  <snc mrk="razi.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Surpriza ampleksa kontrolado kaj <ref tip="super" cel="arest.0ado">arestado</ref>:
      <ekz>
        la kortego instigis sian policon al novaj <tld/>oj
        <fnt>
          <aut>F. Ko&zcaron;&iacute;k, trad. J. Karen kaj J. Vondrou&scaron;ek</aut>,
          <vrk>La plej eminenta inter Pierotoj</vrk>,
          <lok>2001</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_b;&c_l;&c_a;&c_v;&c_a; <klr>(&c_p;&c_a;&c_l;&c_ib;&c_c;&c_e;&c_j;&c_s;&c_k;&c_a;&c_ja;)</klr></trd>
    <trd lng="de">Razzia</trd>
    <trd lng="fr">rafle<klr> (police etc.)</klr></trd>
    <trd lng="pl">nalot policyjny</trd>
    <trd lng="ro">raid al poli&tcomma;iei </trd>
  </snc>
  <trdgrp lng="uk">
    <trd>&c_n;&c_a;&c_b;&c_ib;&c_g;</trd>,
    <trd>&c_n;&c_a;&c_l;&c_ib;&c_t;</trd>,
    <trd>&c_f;&c_a;&c_m;. &c_o;&c_b;&c_l;&c_a;&c_v;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_s;&c_o;&c_v;&c_i;&c_j; &c_a;&c_r;&c_je;&c_sh;&c_t;</trd>
  </trdgrp>
</drv>
</art>

