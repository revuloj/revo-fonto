<drv mrk="mok.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="mok.0i.PSI">
    <dif>
      Mal&scirc;ate, ofende <ref cel="rid.0indigi">ridindigi</ref> iun a&ubreve; ion:
      <ekz>
        la fratinoj &ccirc;ikanis &scirc;in &ccirc;iamaniere, <tld/>is <klr>[&#8230;]</klr>
        <fnt>
          <bib>ElektFab</bib>
          <lok>Cindrulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        li publike <tld/>is mian piecon
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi for&jcirc;etis &ccirc;iujn miajn konsilojn, <klr>[&#8230;]</klr> tial mi
        <tld/>os, kiam timo vin atakos
        <fnt><bib>MT</bib>, <lok>&Sen; 1:25-26</lok></fnt>;
      </ekz>
      <ekz>
        anta&ubreve; ne longe oni tute silentis pri ni kaj poste oni nin <tld/>is, nun oni jam
        &ccirc;ie nin respektas, kiel grandan potencon
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en Dresden en la 17a de a&ubreve;gusto
          1908</vrk>
        </fnt>;
      </ekz>
      <ekz>
        ne <tld/>u mizeron de alia, &ccirc;ar balda&ubreve;
        venos via
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ne <tld/>u riveron neatinginte la teron
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="insult.0i">insulti</ref>,
      <ref cel="kritik.0i">kritiki</ref>,
      <ref cel="rid.mok0i">mokridi</ref>,
      <ref cel="rid.pri0i">priridi</ref>,
      <ref cel="rikan.0i.ridacxi">rikani<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_s;&c_mol;&c_m;&c_ib;&c_h;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_mol;&c_d;&c_z;&c_je;&c_k;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_s;&c_mol;&c_m;&c_je;&c_j;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd>&c_k;&c_p;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&t_-a;&t_pha;&t_yata;&t_tsheg;&t_la;&t_da;&t_tsheg;&t_ra;&t_gata;&t_yata;&t_ga;&t_tsheg;</trd>
  <trdgrp lng="br">
    <trd>goapaat</trd>,
    <trd>ober <ind>goap</ind></trd>,
