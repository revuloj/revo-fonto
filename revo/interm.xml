<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: interm.xml,v 1.30 2024/01/18 15:26:41 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>intermit</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="interm.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="interm.0i.KOMUNE">
    <dif>
      Periode interrompi&gcirc;i, kaj rekomenci&gcirc;i ordinare post egalaj
      intertempoj:
      <ekz>
        la febro <tld/>as;
      </ekz>
      <ekz>
        la konfuzeco de la poemo (a&ubreve; kiel mia poeta amiko esprimis, &gcirc;ia &leftquot;ditiramba
        <tld/>ado&rightquot;) okazigis du preserarojn
        <fnt>
          <bib>SatirRak</bib>
        </fnt>;
      </ekz>
      <ekz>
        la dialogo da&ubreve;ras, sed la kunlaborado <tld/>as
        <fnt>
          <vrk><url ref="&Mondediplo;article2505.html">Le Monde diplomatique' en
          Esperanto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="altern.0i">alterni</ref>,
       <ref cel="eklips.0i">eklipsi</ref>,
       <ref cel="romp.inter0i">interrompi</ref>,
       <ref cel="flagr.0i">flagri</ref>,
       <ref cel="puls.0i">pulsi</ref>,
       <ref cel="trem.0i">tremi</ref>,
       <ref cel="vibr.0i">vibri</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_p;&c_je;&c_r;&c_a;&c_r;&c_y;&c_w;&c_ch;&c_a;&c_s;&c_t;&c_y;&c_m;</ind></trd>,
    <trd>&c_m;&c_je;&c_zh;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>intermitovat</trd>,
    <trd>p&rcaron;eru&scaron;ovat se</trd>,
    <trd>p&rcaron;est&aacute;vat</trd>,
    <trd>vynech&aacute;vat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>intermittieren</trd>,
    <trd>von Zeit zu Zeit <ind>aussetzen</ind></trd>
  </trdgrp>
  <trd lng="en">be intermittent</trd>
  <trd lng="es">intermitir</trd>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>intermittent</ind></trd>,
    <trd>s'<ind>interrompre</ind> p&eacute;riodiquement</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>(id&odblac;ben) <ind>v&aacute;ltakozik</ind></trd>,
    <trd>v&aacute;ltakozva <ind>kihagy</ind></trd>,
    <trd>szaggatottan m&udblac;k&ouml;dik</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x65AD;&#x7D9A;&#12377;&#x308B;  <pr>&#12384;&#12435;&#x305E;&#x304F;&#12377;&#x308B;</pr></trd>,
    <trd>&#38291;&#x6B20;&#12377;&#x308B;  <pr>&#x304B;&#12435;&#12369;&#12388;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>intermitteren</trd>,
    <trd>met <ind>tussenpozen</ind> werken</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przerwom ulega&cacute;</trd>,
    <trd>ustawa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">intermitir</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_m;&c_je;&c_zh;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol; <ind>&c_p;&c_r;&c_je;&c_r;&c_y;&c_v;&c_i;&c_s;&c_t;&c_y;&c_m;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>preru&scaron;ova&tcaron;</trd>,
    <trd>vynech&aacute;va&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_je;&c_r;&c_je;&c_r;&c_i;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_je;&#x43C;&c_je;&c_zh;&#x43E;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&c_b;&c_u;&c_t;&c_i; &#x43F;&c_je;&c_r;&c_je;&c_r;&c_i;&c_v;&c_ch;&c_a;&c_s;&c_t;&c_i;&#x43C;</trd>
  </trdgrp>
</drv>

<drv mrk="interm.0o">
  <kap><tld/>o</kap>
  <snc mrk="interm.0o.KOMUNE">
    <dif>
      Periodo, dum kiu io <tld/>anta interrompi&gcirc;as:
      <ekz>
        li reprenis sian laboron dum la <tld/>oj de lia malsano;
      </ekz>
      <ekz>
        en la unua momento de <tld/>o <klr>(de la socia premo)</klr>
        mi refari&gcirc;as tio, kion la naturo volis
        <fnt><bib>Revadoj</bib>, <lok>Na&ubreve;a promenado</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="interv.0o.MAT">intervalo<sncref/></ref>,
      <ref cel="halt.0o">halto</ref>,
      <ref cel="pauxz.0o">pa&ubreve;zo</ref>,
      <ref cel="period.0o.dauxro">periodo<sncref/></ref>,
      <ref cel="ritm.0o">ritmo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_je;&c_r;&c_a;&c_p;&c_y;&c_n;&c_a;&c_k;</trd>,
    <trd>&c_p;&c_ja;&c_r;&c_e;&c_r;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>p&rcaron;eru&scaron;en&iacute;</trd>,
    <trd>p&rcaron;est&aacute;vka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Unterbrechung</trd>,
    <trd>Aussetzzeit</trd>,
    <trd>Pause</trd>,
    <trd>Sendepause</trd>,
    <trd>Unterlass</trd>
  </trdgrp>
  <trd lng="en">intermission</trd>
  <trd lng="es">intermitencia</trd>
  <trdgrp lng="fr">
    <trd>interruption<klr tip="ind"> (d'un ph&eacute;nom&egrave;ne
intermittent)</klr></trd>,
    <trd>intervalle<klr tip="ind"> (entre deux occurrences)</klr></trd>,
    <trd>r&eacute;pit<klr tip="ind"> (entre deux occurrences)</klr></trd>,
    <trd>rel&acirc;che<klr tip="ind"> (fig., interruption)</klr></trd>,
    <trd>tr&ecirc;ve<klr tip="ind"> (fig., interruption)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>id&odblac;szakos <ind>sz&uuml;net</ind></trd>,
    <trd>id&odblac;szakos <ind>kimarad&aacute;s</ind></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#38291;&#x6B20;  <pr>&#x304B;&#12435;&#12369;&#12388;</pr></trd>,
    <trd>&#x4E2D;&#x4F11;&#x307F;  <pr>&#x306A;&#x304B;&#12420;&#12377;&#x307F;</pr></trd>,
    <trd>&#38291;&#38548;  <pr>&#x304B;&#12435;&#x304B;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">tussenpoos</trd>
  <trdgrp lng="pl">
    <trd>intermisja</trd>,
    <trd>przerwy okres</trd>
  </trdgrp>
  <trd lng="pt">intermit&ecirc;ncia</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_r;&c_y;&c_v;</trd>
  <trdgrp lng="sk">
    <trd>preru&scaron;enie</trd>,
    <trd>prest&aacute;vka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_je;&c_r;&c_je;&c_r;&c_i;&c_v;&c_ch;&c_a;&c_s;&c_t;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_je;&c_r;&c_v;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="interm.0a">
  <kap><tld/>a</kap>
  <snc mrk="interm.0a.KOMUNE">
    <dif>
      <tld lit="I"/>anta:
      <ekz>
        <tld/>a fonto, febro, pulso;
      </ekz>
      <ekz>
        lumturo kun <tld/>aj ekbriloj;
      </ekz>
      <ekz>
        aran&gcirc;i aparaton, kiu dissendu lumon la&ubreve; difinita <tld/>a ritmo
        <fnt>
          <aut>Joh&aacute;n Valano</aut>
          <vrk><url ref="&Tekstaro;?nomo=chu-rakonti-novele">&Ccirc;u rakonti novele? &dash; Ritmaj
          lumoj en la nokto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la sinuslinioj <klr>[de la sunvojo]</klr> estas ie tie <tld/>aj a&ubreve; mankas, kiam
        ekzemple pluvis a&ubreve; venis nuboj
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2012/010750.php">Rivelita: la movado de la suno</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_r;&c_y;&c_w;&c_ch;&c_a;&c_s;&c_t;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>intermitentn&iacute;</trd>,
    <trd>p&rcaron;eru&scaron;ovan&yacute; p&rcaron;est&aacute;vkami</trd>,
    <trd>p&rcaron;er&yacute;van&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>intermittierend</trd>,
    <trd>von Zeit zu Zeit <ind>aussetzend</ind></trd>
  </trdgrp>
  <trd lng="en">intermittent</trd>
  <trd lng="es">intermitente</trd>
  <trd lng="fr">intermittent</trd>
  <trdgrp lng="hu">
    <trd>(id&odblac;ben) <ind>v&aacute;ltakoz&oacute;</ind></trd>,
    <trd>id&odblac;szakosan <ind>kimarad&oacute;</ind></trd>,
    <trd>szaggatott</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x65AD;&#x7D9A;&#30340;&#x306A;  <pr>&#12384;&#12435;&#x305E;&#x304F;&#12390;&#x304D;&#x306A;</pr></trd>,
    <trd>&#38291;&#x6B20;&#30340;&#x306A;  <pr>&#x304B;&#12435;&#12369;&#12388;&#12390;&#x304D;&#x306A;</pr></trd>
  </trdgrp>
  <trd lng="nl">intermitterend</trd>
  <trdgrp lng="pl">
    <trd>intermistyczny</trd>,
    <trd>przerywany</trd>
  </trdgrp>
  <trd lng="pt">intermitente</trd>
  <trd lng="ro">intermitent</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_m;&c_je;&c_zh;&c_a;&c_ju;&c_shch;&c_i;&c_j;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_r;&c_y;&c_v;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ob&ccaron;asn&yacute;</trd>,
    <trd>preru&scaron;ovan&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_je;&c_r;&c_je;&#x43C;&c_ib;&c_zh;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_je;&#x43C;&c_je;&c_zh;&#x43E;&c_v;&c_a;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_je;&c_r;&c_i;&c_v;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#38291;&#x6B47; <pr>ji&#xE0;nxi&emacron;</pr></trd>,
    <trd>&#x65AD;&#x65AD;&#x7EED;&#x7EED; <pr>du&#xE0;ndu&#xE0;nx&#xF9;x&#xF9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="interm.0e">
  <kap><tld/>e</kap>
  <snc mrk="interm.0e.KOMUNE">
    <dif>
      La&ubreve; <tld/>a maniero:
      <ekz>
        la luno <tld/>e brilis inter la nuboj;
      </ekz>
      <ekz>
        la pensoj kirli&gcirc;is <tld/>e tra lia cerbo, inter la ventatakoj
        <fnt>
          <aut>Harald Beijer, trad. Ferenc Szil&aacute;gyi</aut>
          <vrk><url ref="&Tekstaro;?nomo=vespera-rugho-anoncas-ventegon">Vespera ru&gcirc;o
          anoncas ventegon, Liberigita</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e &scirc;i estis savita el tiu hejmetoso per nelonga lo&gcirc;ado kun iu afabla
        parenco, tamen abruptaj vizitoj fare de &scirc;ia patrino ne &ccirc;iam plifeli&ccirc;igis la
        situacion
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2006/009002.php">Memoroj neniam perdas sian mal&gcirc;ojon</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_r;&c_y;&c_w;&c_ch;&c_a;&c_s;&c_t;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>chv&iacute;lemi</trd>,
    <trd>p&rcaron;eru&scaron;en&ecaron;</trd>,
    <trd>p&rcaron;eru&scaron;ovan&ecaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>intermittierend</trd>,
    <trd>von Zeit zu Zeit <ind>aussetzend</ind></trd>
  </trdgrp>
  <trd lng="en">intermittently</trd>
  <trd lng="es">intermitentemente</trd>
  <trdgrp lng="fr">
    <trd>par <ind>intermittence</ind></trd>,
    <trd>par <ind>acc&egrave;s</ind></trd>,
    <trd>par <ind>intervalle</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>(id&odblac;ben) <ind>v&aacute;ltakozva</ind></trd>,
    <trd>szaggatottan</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x65AD;&#x7D9A;&#30340;&#x306B;  <pr>&#12384;&#12435;&#x305E;&#x304F;&#12390;&#x304D;&#x306B;</pr></trd>,
    <trd>&#38291;&#x6B20;&#30340;&#x306B;  <pr>&#x304B;&#12435;&#12369;&#12388;&#12390;&#x304D;&#x306B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>met <ind>tussenpozen</ind></trd>
  </trdgrp>
  <trd lng="pl">z przerwami</trd>
  <trd lng="pt">intermitentemente</trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_r;&c_y;&c_v;&c_i;&c_s;&c_t;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>preru&scaron;ovane</trd>,
    <trd>striedavo</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_je;&c_r;&c_je;&c_r;&c_i;&c_v;&c_ch;&c_a;&c_s;&c_t;&#x43E;</trd>,
    <trd>&c_z; &#x43F;&c_je;&c_r;&c_je;&c_r;&c_v;&c_a;&#x43C;&c_i;</trd>,
    <trd>&c_z; &#x43F;&c_je;&c_r;&c_je;&c_b;&#x43E;&#x44F;&#x43C;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="interm.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="interm.sen0a.KOMUNE">
    <dif>
      <ref tip="vid" cel="romp.sen0a.KOMUNE">Senrompa</ref>,
      <ref tip="vid" cel="kontin3.0a.KOMUNE">kontinua</ref>:
      <ekz>
        ero en &ccirc;en' sen<tld/>a
        <fnt>
          <aut>W. Auld</aut>,
          <vrk><url ref="http://www.sunela.eu/maku/Esperanto/La%20infana%20raso">
          La Infana Raso</url></vrk>,
          <lok>1956</lok>
        </fnt>;
      </ekz>
      <ekz>
        li sidis &ccirc;e apuda tablo, kaj sen<tld/>e rigardis preter mi
        <fnt>
          <aut>Konstanten Krysakov</aut>
          <vrk><url ref="&Monato;/2000/006805.php">Kota akvo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 28</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_je;&c_s;&c_mol;&c_p;&c_je;&c_r;&c_a;&c_p;&c_y;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_mol;&c_p;&c_je;&c_r;&c_a;&c_r;&c_y;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="fr">persistant</trd>
  <trd lng="zh">&#24646;&#x4E45; <pr>h&#xE9;ngji&#x1D4;</pr></trd>
</drv>
</art>
