<vortaro>
<art mrk="$Id: kub1.xml,v 1.17 2023/12/03 09:59:02 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>Kub</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kub1.0o">
  <kap>
    <ofc>8</ofc><tld/>o
    <fnt><aut>Akademio de Esperanto</aut>,
<vrk><url ref="&AdE;/aktoj/aktoj2/8oa.html"> 8a
Oficiala Aldono</url></vrk>, <lok>II.C.1</lok></fnt>
    <fnt><aut>Akademio de Esperanto</aut>,
<vrk><url ref="&AdE;/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>
    <fnt><bib>Viki</bib>, <lok><url ref="&Viki;/Kubo">Kubo</url></lok></fnt>
  </kap>
  <snc mrk="kub1.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <mlg kod="ISO-3166">CU</mlg>
    <dif>
      <ref tip="lst" lst="voko:insuloj" cel="insul.0o">Insulo</ref> en
      Kariba Maro;
      teritorio de Respubliko <tld/>o,
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&scirc;tato</ref> kun
      &ccirc;efurbo
      <ref tip="prt" cel="havan.H0o">Havano</ref>:
      <ekz>
        La 66-a Internacia Junulara Kongreso, IJK okazu en Los Cocos,
        Santa Cruz del Norte, <tld/>o
        <fnt>
          <vrk>Esperanto-Centro Eventoj
            <url ref="http://www.eventoj.hu">Ret-Info</url>
          </vrk>,
       </fnt>;
      </ekz>
      <ekz>
        mesa&gcirc;o portita de <tld/>o al Brazilo okaze de la &scirc;tatestra
        renkonti&gcirc;o
        <fnt>
          <bib>Monato</bib>
          <vrk>Sud-Ameriko</vrk>
          <lok>Ameriko</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="kub.0o">kubo (sesedro, tria potenco)</ref>
  <trd lng="be">&c_K;&c_u;&c_b;&c_a;</trd>
  <trd lng="bg">&c_K;&c_u;&c_b;&c_a;</trd>
  <trd lng="de">Kuba</trd>
  <trd lng="en">Cuba</trd>
  <trd lng="fr">Cuba</trd>
  <trd lng="id">Kuba</trd>
  <trd lng="ja">&#x30AD;&#x30E5;&#x30FC;&#x30D0;  <pr>&#x304D;&#x3085;&#x30FC;&#x3070;</pr></trd>
  <trdgrp lng="pl">
    <trd>Kuba</trd>,
    <trd>Republika <ind>Kuby</ind></trd>
  </trdgrp>
  <trd lng="pt">Cuba</trd>
  <trd lng="uk">&c_K;&c_u;&c_b;&c_a;</trd>
  <trd lng="zh">&#x53E4;&#x5DF4; <pr>G&#x1D4;b&#x101;</pr></trd>
</drv>

<drv mrk="kub1.0ano">
  <kap><tld lit="k"/>ano</kap>
  <snc>
    <dif>
      Civitano de <tld/>o:
      <ekz>
        la varmeco estigas alarmon por <tld lit="k"/>anoj, la altaj temperaturoj de la
        maroj, kiuj &ccirc;irka&ubreve;as la <tld lit="k"/>an insulon, tre favoras ciklonojn
        <fnt>
          <bib>Monato</bib>
          <aut>Yoandra Isabel Rodr&iacute;guez Labrada</aut>
          <vrk><url ref="https://www.monato.be/2009/009753.php">Energiaj ma&scirc;inoj</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_b;&c_ib;&c_n;&c_je;&c_c;</trd>
  <trd lng="de">Kubaner</trd>
  <trdgrp lng="fr">
    <trd>Cubain</trd>
    <trd>Cubaine<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="zh">&#x53E4;&#x5DF4;&#x4EBA; <pr>g&#x1D4;b&#x101;r&#xE9;n</pr></trd>
</drv>

<drv mrk="kub1.0a">
  <kap><tld lit="k"/>a</kap>
  <snc>
    <dif>
    Rilata al la &scirc;tato Kubo
    </dif>
  </snc>
  <ref tip="hom" cel="kub.0a">kuba</ref>
  <trd lng="be">&c_k;&c_u;&c_b;&c_ib;&c_n;&c_s;&c_k;&c_ib;</trd>
  <trd lng="de">kubanisch</trd>
  <trdgrp lng="fr">
    <trd>cubain</trd>
    <trd>cubaine<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>
</art>

