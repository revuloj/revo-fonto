<kap>
  <rad>Promete</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="promete.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">MIT</uzo>
    <dif>
      La&ubreve;vorte &leftquot;anta&ubreve;pensanto&rightquot;;
      en la helena mitologio, <ref tip="vid" cel="titan.0o">titano</ref>,
      kreinto kaj protektanto de la homaro:
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> per la helpo de Atena, supreniris al &ccirc;ielo,
        kaj ekbruligis sian tor&ccirc;on &ccirc;e la &ccirc;aro de la suno, kaj subenportis fajron al homo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=29671">Prometeo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
    <tld/>o estis figuro el la greka mitologio, kiu &scirc;telis fajron de la dioj kaj donacis &gcirc;in al la
    homaro
    <fnt>
      <aut>Anna L&ouml;wenstein</aut>
      <vrk><url ref="&Monato;/2023/013126.php?p">Tragedia rakonto</url></vrk>
      <bib>Monato</bib>
      <lok>2023/03, p. 30</lok>
      </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_P;&c_r;&c_a;&c_m;&c_e;&c_t;&c_e;&c_j;</trd>
  <trdgrp lng="cs">
    <trd>Prometheus</trd>,
    <trd>Prom&eacute;theus</trd>
  </trdgrp>
   <trd lng="de">Prometheus</trd>
  <trdgrp lng="fa">
    <trd>&f_p;&a_r;&a_w;&a_m;&a_t;&a_y_hamza;&a_w;&a_s;</trd>,
    <trd>&f_p;&a_r;&a_w;&a_m;&a_t;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">Prom&eacute;th&eacute;e</trd>
  <trd lng="it">Prometeo</trd>
  <trd lng="ja">&#x30D7;&#x30ED;&#x30E1;&#x30C6;&#x30A6;&#x30B9;</trd>
  <trd lng="nl">Prometheus</trd>
  <trd lng="pl">Prometeusz</trd>
  <trd lng="ru">&c_P;&c_r;&c_o;&c_m;&c_je;&c_t;&c_je;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>Prometheus</trd>,
    <trd>Prom&eacute;theus</trd>
  </trdgrp>
  <trd lng="uk">&#x41F;&c_r;&#x43E;&#x43C;&c_je;&c_t;&c_je;&c_j; (&c_t;&c_i;&c_t;&c_a;&#x43D;)</trd>
  <trdgrp lng="zh">
    <trd>&#x666E;&#x7F57;&#x7C73;&#x4FEE;&#x65AF; <pr>p&#x1D4;lu&#xF3;m&#x1D0;xi&#x16B;s&#x12B;</pr></trd>,
    <trd>&#x666E;&#x7F85;&#x7C73;&#x4FEE;&#x65AF; <pr>p&#x1D4;lu&#xF3;m&#x1D0;xi&#x16B;s&#x12B;</pr></trd>
  </trdgrp>
</drv>
</art>

