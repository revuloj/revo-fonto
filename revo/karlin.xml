<kap>
  <rad>karlin</rad>/o
</kap>
<drv mrk="karlin.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio <ref tip="malprt" cel="astera.0oj">asteracoj</ref>
      (<trd lng="la">Carlina</trd>):
      <ekz>
        <tld/>o estas genro de du- kaj plur-jaraj herboj kun plume diverse entran&ccirc;aj,
        dorne dentaj folioj kaj kun unu a&ubreve; kelkaj, grandaj tubetfloraj kapituloj,
        kies brakteoj de la interna parto de la involukro aspektas kiel krono el
        largetfloroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=33516">Karlino</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="kard.0o.BOT">kardo<sncref/></ref>,
       <ref cel="dipsak.0o">dipsako</ref>
    </refgrp>
    <bld lok="&WCU;/thumb/7/7d/Image-Carlina_acanthifolia_0003.jpg/640px-Image-Carlina_acanthifolia_0003.jpg" prm="CC-BY-SA-3.0"><tld/>o (Carlina acanthifolia)
        <fnt>
          <lok><url ref="&WCW;/File:Image-Carlina_acanthifolia_0003.jpg">&WCW;/File:Image-Carlina_acanthifolia_0003.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_u;&c_r;&c_o;&c_ch;&c_n;&c_ib;&c_k;</trd>
  <trd lng="ca">carlina</trd>
  <trdgrp lng="de">
    <trd>Eberwurz</trd>,
    <trd>Silberdistel</trd>
  </trdgrp>
  <trd lng="fa">&a_s;&a_r;&a_T;&a_l;&a_A;&f_y;&f_y;</trd>
  <trd lng="fr">carline</trd>
  <trd lng="hu">b&aacute;bakal&aacute;cs<klr tip="ind">
(n&ouml;v&eacute;ny)</klr></trd>
  <trd lng="ja">&#x30C1;&#x30E3;&#x30DC;&#x30A2;&#x30B6;&#x30DF;</trd>
  <trd lng="nl">driedistel</trd>
  <trd lng="pl">dziewi&eogonek;&cacute;si&lstroke;</trd>     
  <trd lng="ro">carlina</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_l;&c_ju;&c_ch;&c_je;&c_l;&c_i;&c_s;&c_t;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_ju;&c_ch;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="uk">&#x432;&#x456;&#x434;&#x43A;&#x430;&#x441;&#x43D;&#x438;&#x43A;</trd>
</drv>
