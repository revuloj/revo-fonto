<vortaro>
  <art mrk="$Id: karakas.xml,v 1.7 2023/12/03 08:00:23 afido Exp $">
    <kap>
      <rad>Karakas</rad>/o
    </kap>

<drv mrk="karakas.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      La <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&ccirc;efurbo</ref>
      de <ref tip="malprt" cel="venezuel.V0o">Venezuelo</ref>:
      <ekz>
        <tld/>o trovi&gcirc;as en la norda parto de la lando proksime de <ref cel="mar.Kariba0o">Kariba
        Maro</ref>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18544">Karakaso</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        malliberigita en <tld/>o <klr>[&#x2026;]</klr>, s-ro <nom>Posada Carriles</nom>, ankora&ubreve; ne
        ju&gcirc;ita, eskapas en 1985 kun helpo de la Kuba-Usona Nacia Fonda&jcirc;o kaj de la CIA
        <fnt>
          <vrk><url ref="&Mondediplo;article1351.html">Posada Carriles
          (Luis)</url></vrk>
          <lok>Le Monde diplomatique en Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;ancoj de dungi&gcirc;o estas tiel malmultaj ke, en a&ubreve;gusto 2018, <tld/>o starigis
        la aeran kaj teran pontojn de &leftquot;La reveno al la Patrolando&rightquot;
        <fnt>
          <vrk><url ref="&Mondediplo;article2760.html">Sur la vojo de la venezuelaj
          migrantoj</url></vrk>
          <lok>Le Monde diplomatique en Esperanto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Caracas</trd>
    <trd lng="en">Caracas</trd>
    <trd lng="fr">Caracas</trd>
    <trd lng="nl">Caracas</trd>
  </snc>
  <trd lng="be">&c_K;&c_a;&c_r;&c_a;&c_k;&c_a;&c_s;</trd>
  <trd lng="ja">&#x30AB;&#x30E9;&#x30AB;&#x30B9;</trd>
  <trd lng="uk">&#x41A;&c_a;&c_r;&c_a;&#x43A;&c_a;&c_s;</trd>
  <trdgrp lng="zh">
    <trd>&#x52A0;&#x62C9;&#x52A0;&#x65AF; <pr>Ji&#x101;l&#x101;ji&#x101;s&#x12B;</pr></trd>,
    <trd>&#x5361;&#x62C9;&#x5361;&#x65AF; <pr>K&#x1CE;l&#x101;k&#x1CE;s&#x12B;</pr></trd>
  </trdgrp>
</drv>

