<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sklerot.xml,v 1.5 2022/08/26 07:34:36 revo Exp $">
<kap>
  <rad>sklerot</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sklerot.0o">
  <kap><tld/>o</kap>
  <snc mrk="sklerot.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Blanka, fibreca membrano, videbla &ccirc;irka&ubreve; la iriso kaj 
      &ccirc;irka&ubreve;anta la ceteran
      <ref tip="malprt" cel="okul.0o.ANA">okul</ref>globon:
      <ekz>
        la <tld/>o <klr>...</klr> estas, en sia anta&ubreve;a 
        parto, en kontinueco kun la korneo
        <fnt>
          <aut>Internacia Naturkuraca Asocio</aut>,
          <vrk><url ref="http://www.ina-asocio.info/naturkuracado15.html">
          Okulaj malsanoj</url></vrk>,
          <lok>[2012?]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>scl&egrave;re</trd>,
      <trd>scl&eacute;rotique</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_l;&c_je;&c_r;&c_a;</trd>,
    <trd>&c_c;&c_mol;&c_v;&c_je;&c_r;&c_d;&c_a;&c_v;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>o&#x10D;n&#xED; b&#x11B;lima</trd>,
    <trd>skl&#xE9;ra</trd>
  </trdgrp>
  <trd lng="sk">skl&#xE9;ra</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43A;&#x43B;&c_je;&c_r;&c_a;</trd>,
    <trd>&c_b;&c_ib;&#x43B;&#x43A;&#x43E;&c_v;&c_a; &#x43E;&c_b;&#x43E;&#x43B;&#x43E;&#x43D;&#x43A;&c_a;
    &#x43E;&#x43A;&c_a;</trd>,
    <trd>&c_s;&#x43A;&#x43B;&c_je;&c_r;&#x43E;&c_c;&c_ib;&c_j;</trd>
  </trdgrp>
</drv>
</art>

