<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>sova&gcirc;</rad>/a
</kap>
<drv mrk="sovagx.0a">
   <kap><ofc>*</ofc><tld/>a</kap>
   <snc>
     <dif>
       Necivilizita (pp homo), ne hejmigita (pp besto), ne kulturita (pp
       veg.):
       <ekz>
         <tld/>a bovo;
       </ekz>
       <ekz>
         <tld/>a greno<fnt>Z</fnt>,
       </ekz>
       <ekz>
         vinberujo.
       </ekz>
     </dif>
     <trd lng="pl">dziki</trd>
   </snc>
   <snc>
     <dif>
       Akre kaj malagrable impresanta la sentumojn pro sia krudeco kaj
       maldelikateco:
       <ekz>
         <tld/>a koloro;
       </ekz>
       <ekz>
         <tld/>e vestita;
       </ekz>
       <ekz>
         Volap&uuml;k sonas <tld/>e kaj maldelikate<fnt>Z</fnt>;
       </ekz>
       <ekz>
         al &ccirc;io oni povas alkutimi&gcirc;i, kaj kio hiera&ubreve; &scirc;ajnis <tld/>a,
         tio morga&ubreve; aperas kiel io tute natura kaj bela<fnt>Z</fnt>.
       </ekz>
     </dif>
   </snc>
   <snc>
     <dif>
       Senbride fortega:
       <ekz>
         <tld/>a ludo, gesto;
       </ekz>
       <ekz>
         <tld/>e pu&scirc;i iun;
       </ekz>
       <ekz>
         <tld/>torenta pluvo<fnt>Z</fnt>;
       </ekz>
       <ekz>
         la elementoj ekstere <tld/>as<fnt>Z</fnt>
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="uragan.0a">uragana</ref>,
       <ref cel="torent.0a">torenta</ref>,
       <ref cel="lavang">lavanga</ref>,
       <ref cel="furioz.0a">furioza</ref>,
       <ref cel="impet.0ega">impetega</ref>,
       <ref cel="brid.sen0a">senbrida</ref>,
       <ref cel="cxen">el&ccirc;eni&gcirc;i</ref>.
     </refgrp>
     <trd lng="ru">&c_s;&c_v;&c_i;&c_r;&c_je;&c_p;&c_y;&c_j;</trd>
   </snc>
   <trd lng="be">&c_d;&c_z;&c_ib;&c_k;&c_ib;</trd>
   <trd lng="de">wild</trd>
   <trd lng="fr">sauvage</trd>
   <trdgrp lng="hu">
     <trd>vas</trd>,
     <trd>nyers</trd>,
     <trd>durva</trd>
   </trdgrp>
   <trdgrp lng="nl">
     <trd>wild</trd>,
     <trd>woest</trd>
   </trdgrp>
   <trd lng="ru">&c_d;&c_i;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="sovagx.0ejo">
   <kap><tld/>ejo</kap>
   <snc>
     <dif>
       Necivilizita kaj nekulturita lando.
     </dif>
   </snc>
   <trd lng="be">&c_d;&c_z;&c_ib;&c_k;&c_a;&c_ja; &c_m;&c_ja;&c_s;&c_c;&c_o;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
   <trd lng="de">Wildnis</trd>
   <trd lng="hu">vadon</trd>   
   <trd lng="nl">wildernis</trd>
   <trd lng="pl">dzicz</trd>
   <trd lng="ru">&c_d;&c_i;&c_k;&c_a;&c_ja; &c_m;&c_je;&c_s;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="sovagx.mal0a">
   <kap>mal<tld/>a</kap>
   <snc>
     <dif>
       Kulturita a&ubreve; hejmigita.
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_a;&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_a;&c_n;&c_y;</trd>,
     <trd>&c_p;&c_r;&c_y;&c_r;&c_u;&c_ch;&c_a;&c_n;&c_y;</trd>,
     <trd>&c_c;&c_y;&c_v;&c_ib;&c_l;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
   </trdgrp>
   <trd lng="de">zahm</trd>
   <trd lng="fr">civilis&eacute;</trd>
   <trdgrp lng="hu">
     <trd>szel&iacute;d</trd>,
     <trd>kezes</trd>
   </trdgrp>
   <trdgrp lng="nl">
     <trd>tam</trd>,
     <trd>mak</trd>
   </trdgrp>
   <trdgrp lng="ru">
    <trd>&c_o;&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_d;&c_o;&c_m;&c_a;&c_sh;&c_n;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
   </trdgrp>
</drv>
</art>
