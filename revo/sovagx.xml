<drv mrk="sovagx.0i">
  <kap><tld/>i</kap>
  <snc mrk="sovagx.0i.KOMUNE">
    <dif>
      Abrupte, senbride efiki, kiel natura&jcirc;o:
      <ekz>
        la elementoj <klr>[&#8230;]</klr> ekstere <tld/>as
        <fnt>
          <bib>BdV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxen">el&ccirc;eni&gcirc;i</ref>
    </refgrp>
    <trd lng="de">toben</trd>
    <trdgrp lng="zh">
    <trd>&#x72C2;&#x543C; <pr>ku&#xE1;ngh&#x1D2;u</pr></trd>,
    <trd>&#x72C2;&#x53F7; <pr>ku&#xE1;ngh&#xE0;o</pr></trd>,
    <trd>&#x72C2;&#x865F; <pr>ku&#xE1;ngh&#xE0;o</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_l;&c_ju;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="es">
