<drv mrk="sovagx.mal0igi">
  <kap>mal<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sovagx.mal0igi.sentimigi">
    <dif>
      Igi <ref cel="sovagx.mal0a.sentima">mal<tld/>a<sncref/></ref>:
      <ekz>
        dividante sian panon kun urso, li mal<tld/>igis &gcirc;in
        <fnt>
          <bib>Monato</bib> <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2009/009756.php">Praa religia temo</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz><ekz>
        la granda besto apartenis al speco, kiun la ru&gcirc;aj
        homoj neniam mal<tld/>igas
        <fnt>
          <aut>E. R. Burroughs, trad. K. R. C. Sturmer</aut>,
          <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0029/index.php">
          Princino de Marso</url></vrk>,
          <lok>1938</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_i;&c_r;&c_u;&c_ch;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_r;&c_u;&c_ch;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sovagx.mal0igi.specion">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Adapti <klr>(vegetalan a&ubreve; animalan specion)</klr> al kultivado a&ubreve;
      bredado kaj konsumado en homa mastrumado:
      <ekz>
        la&ubreve; la hipotezo de la pruraj originoj de niaj kolomboj, oni
        devas supozi ke almena&ubreve; sep a&ubreve; ok specioj estis tiom &gcirc;isfine
        mal<tld/>igitaj en antikva tempo per duoncivilizitaj homoj, ke
        ili estas tre fekundaj en kaptiteco
        <fnt>
          <bib>OriginoDeSpecioj</bib>,
          <lok>&ccirc;apitro 1a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_o;&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_d;&c_o;&c_m;&c_a;&c_sh;&c_n;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
      <!--ekz>
        li ofte estis &ccirc;irka&ubreve;ligita per katenoj kaj &ccirc;enoj, <klr>[&ccirc;ar]</klr> neniu
        kapablis mal<tld/>igi lin
        <fnt><bib>NT</bib> <lok>&Mar; 5:4</lok></fnt>;
      </ekz-->
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_r;&c_u;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_r;&c_a;&c_h;&c_m;&c_a;&c_n;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">z&auml;hmen</trd>
  <trd lng="en">tame</trd>
