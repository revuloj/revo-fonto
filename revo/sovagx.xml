<drv mrk="sovagx.0i">
  <kap><tld/>i</kap>
  <snc mrk="sovagx.0i.KOMUNE">
    <dif>
      Abrupte, senbride efiki, kiel natura&jcirc;o:
      <ekz>
        la elementoj <klr>[&#x2026;]</klr> ekstere <tld/>as
        <fnt>
          <bib>BdV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxen">el&ccirc;eni&gcirc;i</ref>
    </refgrp>
    <trd lng="de">toben</trd>
  </snc>
  <trdgrp lng="es">
    <trd>asalvajar</trd>,
    <trd>embravecer</trd>
  </trdgrp>
</drv>

<drv mrk="sovagx.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="sovagx.0ejo.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      Necivilizita kaj nekultivata lando:
      <ekz>
        mi hazarde vidis du infanojn ludantajn en tiu <tld/>ejo el balaa&jcirc;oj
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
      <ekz>
        la verda valo restas malanta&ubreve;e kaj ekas seka <tld/>ejo kun kaktoj kaj
        tolao-arbetoj
        <fnt>
          <bib>Monato</bib>
          <aut>Marta Sol&iacute;s</aut>
          <vrk><url ref="https://www.monato.be/2005/008399.php">Unika fervojo en Andoj</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni volas en &ccirc;i tiu <tld/>ejo konstrui domon al ni
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_d;&c_z;&c_ib;&c_k;&c_a;&c_ja; &c_m;&c_ja;&c_s;&c_c;&c_o;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
   <trd lng="de">Wildnis</trd>
   <trd lng="es">naturaleza <ind>salvaje</ind></trd>   
   <trdgrp lng="fr">
     <trd>d&eacute;sert</trd>,
     <trd>nature sauvage</trd>
   </trdgrp>
   <trd lng="hu">vadon</trd>   
   <trd lng="nl">wildernis</trd>
   <trd lng="pl">dzicz</trd>
   <trd lng="ru">&c_d;&c_i;&c_k;&c_a;&c_ja;
   &c_m;&c_je;&c_s;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="sovagx.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="sovagx.mal0a.BIO">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Kultivata a&ubreve; <ref tip="vid" cel="hejm.0igita">hejmigita</ref>:
      <ekz>
        mi havas mal<tld/>an amatinon, kiu libere promenadas en la palaco
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>a leono en ka&gcirc;o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>;
      </ekz>
       <ekz>
         trajtoj, kiuj aperas &ccirc;e la maskloj de niaj
         mal<tld/>aj rasoj ofte estas transdonataj
         ekskluzive, a&ubreve; &ccirc;efe, al maskloj
         <fnt>
           <bib>OriginoDeSpecioj</bib>,
           <lok>&ccirc;apitro 1a</lok>
         </fnt>.
       </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
     <trd>&c_a;&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_a;&c_n;&c_y;</trd>,
     <trd>&c_p;&c_r;&c_y;&c_r;&c_u;&c_ch;&c_a;&c_n;&c_y;</trd>,
     <trd>&c_c;&c_y;&c_v;&c_ib;&c_l;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
   </trdgrp>
   <trd lng="de">zahm</trd>
   <trdgrp lng="fr">
     <trd>cultiv&eacute;<klr> (plante)</klr></trd>,
     <trd>domestique<klr> (animal)</klr></trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>szel&iacute;d</trd>,
     <trd>kezes</trd>
   </trdgrp>
   <trdgrp lng="nl">
     <trd>tam</trd>,
     <trd>mak</trd>
   </trdgrp>
   <trdgrp lng="pl">
     <trd>udomowiony<klr> (o zwierz&eogonek;ciu)</klr></trd>,
     <trd>oswojony<klr> (o zwierz&eogonek;ciu lub
     cz&lstroke;owieku)</klr></trd>,
     <trd>uprawny <klr>(o polu)</klr></trd>
   </trdgrp>
   <trdgrp lng="ru">
    <trd>&c_o;&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_d;&c_o;&c_m;&c_a;&c_sh;&c_n;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
   </trdgrp>
</drv>

<drv mrk="sovagx.mal0igi">
  <kap>mal<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sovagx.mal0igi.KOMUNE">
    <dif>
      Igi mal<tld/>a:
      <ekz>
        li ofte estis &ccirc;irka&ubreve;ligita per katenoj kaj &ccirc;enoj, <klr>[&ccirc;ar]</klr> neniu
        kapablis mal<tld/>igi lin
        <fnt>
          <bib>NT</bib>
          <lok>Marko 5:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        dividante sian panon kun urso, li mal<tld/>igis &gcirc;in
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="https://www.monato.be/2009/009756.php">Praa religia temo</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        la granda besto apartenis al speco, kiun la ru&gcirc;aj 
        homoj neniam mal<tld/>igas
        <fnt>
          <aut>E. R. Burroughs, trad. K. R. C. Sturmer</aut>,
          <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0029/index.php">
          Princino de Marso</url></vrk>,
          <lok>1938</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">z&auml;hmen</trd>
  <trd lng="en">tame</trd>
  <trdgrp lng="fr">
    <trd>apprivoiser</trd>,
    <trd>domestiquer</trd>
  </trdgrp>
</drv>
