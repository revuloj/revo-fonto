<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: gadulk.xml,v 1.3 2024/07/29 09:29:27 afido Exp $">
    <kap>
      <rad>gadulk</rad>/o
    </kap>

<drv mrk="gadulk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Bulgara <ref tip="super" cel="liut.0o">liuto</ref>
      ludata per ar&ccirc;o:
      <ekz>
        danci sub la sonoj de originala bulgara muzikilo <tld/>o (ribeko)
        <fnt>
          <aut>Dimitar Ha&gcirc;iev</aut>
          <vrk><url ref="&Monato;/2008/009495.php"
              >Adia&ubreve;o al la dancantaj ursoj</url></vrk>
              <bib>Monato</bib>
          <lok>2008/10, p. 18</lok>
        </fnt>;
      </ekz>
      <ekz>
        gadulko <klr>[&#8230;]</klr> havas tri kordojn
        <klr>[kiujn]</klr> oni agordas en A3,
        E3, A4
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=579071"
              >Gadulko</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_g;&c_malmol;&c_d;&c_u;&c_l;&c_k;&c_a;</trd>
    <trd lng="de">Gadulka</trd>
    <trd lng="en">gadulka</trd>
    <trd lng="fr">gadoulka</trd>
    <trdgrp lng="nl">
      <trd>gdulka</trd>,
      <trd>gadoelka</trd>
    </trdgrp>
  </snc>
<trdgrp lng="be">
  <trd>&c_g;&c_a;&c_d;&c_u;&c_l;&c_k;&c_a;</trd>,
  <trd>&c_g;&c_u;&c_d;&c_u;&c_l;&c_k;&c_a;</trd>
</trdgrp>
</drv>

  </art>

