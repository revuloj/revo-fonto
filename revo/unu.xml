  </trdgrp>
  <trdgrp lng="de">
        <trd>zuerst</trd>,
        <trd>als erstes</trd>,
        <trd>erstens</trd>
     </trdgrp>
  <trdgrp lng="fi">
        <trd>ensin</trd>,
        <trd>ensiksi</trd>
     </trdgrp>
  <trdgrp lng="fr">
        <trd>premi&egrave;rement</trd>,
