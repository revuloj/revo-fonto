     </trdgrp>
</drv>

    <drv mrk="unu.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Abstrakta kvanto, konsiderata kiel la elementa nombro,
          kiu servas por formi la ceterajn:
          <ekz>
            la deko konsistas el dek <tld/>oj<fnt>B</fnt>;
         </ekz><ekz>
            skribi ciferon en la kolono de la <tld/>oj;
         </ekz><ekz>
            ne estimante apartajn <tld/>ojn, ni neniam venos al grandaj
            nombroj<fnt>Z</fnt>.
         </ekz>
       </dif>
        <refgrp tip="vid">
          <ref cel="gut.0o">guto</ref>,
          <ref cel="prezen1.numa0o">n-uma prezento</ref>.
       </refgrp>
        <trdgrp lng="fi">
          <trd>ykk&ouml;nen</trd>,
          <trd>yksikk&ouml;</trd>
       </trdgrp>
        <trd lng="hu">egyes <klr tip="ind">(sz&aacute;m)</klr></trd>
     </snc>
      <snc mrk="unu.0o.mezuro">
        <dif>
          <ref tip="dif" cel="unu.mezur0o">Mezur<tld/>o</ref>:
          <ekz>
            la <ind>Internacia Sistemo de <tld/>oj</ind>;
            <trd lng="br">Reizhiad Unanenno&ugrave; Etrebroadel</trd>
            <trd lng="ca">Sistema Internacional d'Unitats (SI)</trd>
            <trdgrp lng="fi">
              <trd>kansainv&auml;linen
                yksikk&ouml;j&auml;rjestelm&auml;</trd>,
              <trd>SI-j&auml;rjestelm&auml;</trd>
           </trdgrp>
            <trdgrp lng="fr">
              <trd>SI</trd>,
              <trd>Syst&egrave;me International d'Unit&eacute;s</trd>
           </trdgrp>
            <trdgrp lng="hu">
              <trd>SI</trd>,
              <trd>Nemzetk&ouml;zi
                M&eacute;rt&eacute;kegys&eacute;grendszer</trd>
           </trdgrp>
            <trd lng="id"><ind>Satu</ind>an Internasional</trd>
            <trd lng="nl">SI-stelsel</trd>
            <trd lng="no">SI-systemet</trd>
            <trdgrp lng="pl">
              <trd>SI</trd>,
              <trd>Mi&eogonek;dzynarodowy Uk&lstroke;ad Jednostek
              Miar</trd>
           </trdgrp>
            <trdgrp lng="ru">
              <trd>&c_S;&c_I;</trd>,
              <trd>&c_M;&c_je;&c_zh;&c_d;&c_u;&c_n;&c_a;&c_r;&c_o;&c_d;&c_n;&c_a;&c_ja;
                 &c_s;&c_i;&c_s;&c_t;&c_je;&c_m;&c_a;
                 &c_je;&c_d;&c_i;&c_n;&c_i;&c_c;</trd>
           </trdgrp>
         </ekz>
         <ekz>
            <uzo tip="fak">AST</uzo>
            unu <ind>astronomia <tld/>o</ind>, kies simbolo estas au,
            estas la avera&gcirc;a distanco inter la Suno kaj la Tero
            <fnt><bib>Kosmo</bib></fnt>;
            <trd lng="br">unanenn astronomek</trd>
            <trd lng="ca">unitat astron&ograve;mica (UA)</trd>
            <trd lng="en">astronomical unity</trd>
            <trd lng="fr">unit&eacute; astronomique</trd>
            <trd lng="hu">csillag&aacute;szati egys&eacute;g</trd>
            <trd lng="id"><ind>satu</ind>an astronomi</trd>
            <trd lng="nl">astronomische eenheid</trd>
            <trd lng="no">astronomisk enhet</trd>
            <trd lng="ru">&c_a;&c_s;&c_t;&c_r;&c_o;&c_n;&c_o;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja;
              &c_je;&c_d;&c_i;&c_n;&c_i;&c_c;&c_a;<klr> (&c_d;&c_l;&c_i;&c_n;&c_y;)</klr>
            </trd>
         </ekz>
         <ekz>
            <ind>temp<tld/>o</ind>.
            <trd lng="hu">id&odblac;egys&eacute;g</trd>
         </ekz>
        </dif>
        <trd lng="fi"><klr tip="amb">(mitta)</klr>yksikk&ouml;</trd>
        <trd lng="hu">m&eacute;rt&eacute;kegys&eacute;g</trd>
        <trd lng="pl">jednostka</trd>
        <trd lng="sv">enhet</trd>
     </snc>
      <snc mrk="unu.0o.neuxtra">
        <uzo tip="fak">MAT</uzo>
        <fnt><bib>KompLeks</bib>, <lok>ne&ubreve;tra
        elemento</lok></fnt>
        <dif>
          <ref tip="super" cel="neuxtr.0aelemento">Ne&ubreve;tra 
            elemento</ref>
          rilate al multiplike signata
          <ref cel="operac.0o.MAT">operacio</ref>.
       </dif>
        <rim>
          Tiun terminon subtenas nerekte anka&ubreve;
          <fnt><bib>PIV2</bib>, <lok>grupo</lok></fnt>, kiu tamen
          konservas por &gcirc;i la nematematikan difinon, troveblan jam
          en
          <fnt><bib>PIV1</bib></fnt>. Multaj aliaj fontoj uzas
          &ccirc;i-sence nur
          neologismon <ctl>unito</ctl>
          (ekz-e <fnt><bib>EKV</bib>, <lok>&FE;447</lok></fnt>), kies
          avanta&gcirc;on ni tamen ne kapablas vidi.
       </rim>
        <trd lng="de">Einselement</trd>
        <trd lng="en">unit<klr tip="ind"> (element)</klr></trd>
        <trdgrp lng="fi">
          <trd>ykk&ouml;salkio</trd>,
          <trd>neutraalialkio</trd>
       </trdgrp>
        <trd lng="hu">egys&eacute;gelem</trd>
        <trdgrp lng="pl">
          <trd>jedynka<klr tip="ind"> (element neutralny)</klr></trd>,
          <trd>element jednostkowy</trd>
       </trdgrp>
     </snc>
  <trd lng="be">&c_a;&c_d;&c_z;&c_ib;&c_n;&c_k;&c_a;</trd>
  <trd lng="br">unanenn</trd>
  <trd lng="ca">unitat</trd>
  <trdgrp lng="cs">
    <trd>jedni&ccaron;ka</trd>,
    <trd>jednotka</trd>,
    <trd>jednotkov&yacute; prvek</trd>
  </trdgrp>
  <trd lng="de">Einheit</trd>
  <trd lng="fr">unit&eacute;</trd>
  <trd lng="he">&yod;&het;&yod;&dalet;&he;</trd>
  <trd lng="id">unit</trd>
  <trd lng="it">unit&agrave;<klr tip="amb"> (misura o
          elemento)</klr></trd>
  <trd lng="nl">eenheid</trd>
  <trd lng="no">enhet</trd>
  <trd lng="pt">unidade</trd>
  <trd lng="ru">&c_je;&c_d;&c_i;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">jednotka miery</trd>
</drv>

    <drv mrk="unu.0eco">
      <kap><tld/>eco</kap>
      <snc mrk="unu.0eco.unikeco">
        <dif>
          Eco de tio, kio estas nur <tld/>:
          <ekz>
            la dogmo pri la <tld/>eco de Dio.
         </ekz>
       </dif>
        <trd lng="fi">ykseys</trd>
        <trdgrp lng="hu">
          <trd>egyed&uuml;lis&eacute;g</trd>,
          <trd>egyed&uuml;lval&oacute;s&aacute;g</trd>
       </trdgrp>
        <trd lng="it">unicit&agrave;</trd>
        <trd 
lng="ru">&c_je;&c_d;&c_i;&c_n;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
     </snc>
      <snc mrk="unu.0eco.kohero">
        <dif>
          Karaktero de tio, kies &ccirc;iuj partoj kunligi&gcirc;as por
          konsistigi nur <tld/> tuton:
          <ekz>
            la <tld/>eco estas la plej grava principo de internacia
            lingvo;
         </ekz><ekz>
            <ind><tld/>eco donas fortecon</ind>
            <trd lng="ca">la uni&oacute; fa la for&ccedil;a</trd>
            <trd lng="fr">l'union fait la force</trd>
            <fnt><bib>PrV</bib></fnt>;
         </ekz><ekz>
            romano, kiu havas <tld/>econ
            (<ref tip="vid" cel="kompon.0i">komponi</ref>);
         </ekz><ekz>
            rompi la &scirc;tatan, nacian <tld/>econ.
         </ekz>
       </dif>
        <trd
        lng="be">&c_a;&c_d;&c_z;&c_ib;&c_n;&c_s;&c_t;&c_v;&c_a;</trd>
        <trd lng="fi">yhten&auml;isyys</trd>
        <trdgrp lng="he">
          <trd>&alef;&het;&yod;&dalet;&vav;&tav;</trd>,
          <trd>&alef;&het;&dalet;&vav;&tav;</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>egys&eacute;g</trd>,
          <trd>egys&eacute;gess&eacute;g</trd>
        </trdgrp>
        <trd lng="ru">&c_je;&c_d;&c_i;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>
        <trd lng="sv">enhetlighet</trd>
     </snc>
  <trdgrp lng="ca">
        <trd>uni&oacute;</trd>,
        <trd>unicitat</trd>
     </trdgrp>
  <trdgrp lng="cs">
    <trd>jednota</trd>,
    <trd>jednotnost</trd>,
    <trd>jednozna&ccaron;nost</trd>
  </trdgrp>
  <trdgrp lng="de">
        <trd>Einheit</trd>,
        <trd>Einheitlichkeit</trd>
     </trdgrp>
  <trd lng="es">unidad<klr tip="amb"> (la unidad de varias
      cosas)</klr></trd>
  <trd lng="fr">unit&eacute;<klr tip="ind"> (qualit&eacute;)</klr></trd>
  <trdgrp lng="id">
        <trd>[<ind>satu</ind>] kesatuan</trd>,
        <trd>[<ind>satu</ind>] persatuan</trd>
      </trdgrp>
  <trd lng="it">unit&agrave;<klr tip="amb"> (unione)</klr></trd>
  <trd lng="nl">eenheid<klr tip="ind"> (samenhorigheid)</klr></trd>
  <trd lng="no">enighet</trd>
  <trdgrp lng="sk">
    <trd>jednota</trd>,
    <trd>jednotnos&tcaron;</trd>
  </trdgrp>
</drv>

    <drv mrk="unu.0igi">
      <kap><tld/>igi</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Kunigi plurajn objektojn en unu tuton:
          <ekz>
            la animo estas <tld/>igita kun la korpo;
         </ekz><ekz>
            <tld/>igi la fortojn<fnt>B</fnt>;
         </ekz><ekz>
            &scirc;i <tld/>igis en si la &ccirc;armon kaj la
            sa&gcirc;on.
         </ekz>
       </dif>
        <trdgrp lng="ca">
          <trd>unir</trd>,
          <trd>reunir</trd>
       </trdgrp>
     </snc>
      <snc>
        <dif>
          Kunigi per politika, religia, komerca a&ubreve; simila ligo:
          <ekz>
            la <tld/>igita re&gcirc;lando de Granda Britujo kaj Irlando;
         </ekz><ekz>
            la <tld/>igitaj &scirc;tatoj de Norda Ameriko (Usono);
         </ekz><ekz>
            ni &jcirc;uris labori por <ind>re<tld/>igi</ind> l'
            homaron
            <fnt>
              <aut>L. L. Zamenhof</aut>
              <vrk>Pre&gcirc;o sub la Verda Standardo</vrk>
            </fnt>.
            <trdgrp lng="ca">
              <trd>rejuntar</trd>,
              <trd>reunificar</trd>
           </trdgrp>
         </ekz>
       </dif>
        <refgrp tip="vid">
          <ref cel="fand.kun0i">kunfandi</ref>,
          <ref cel="miks">kunmiksi</ref>
       </refgrp>
        <trd lng="ca">unificar</trd>
        <trd lng="sv">f&ouml;rena</trd>
     </snc>
  <trdgrp lng="be">
        <trd>&c_a;&c_b;'&c_ja;&c_d;&c_n;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_z;&c_l;&c_u;&c_ch;&c_y;&c_c;&c_mol;</trd>,
        <trd>&c_z;&c_mol;&c_v;&c_ja;&c_z;&c_a;&c_c;&c_mol;</trd>
     </trdgrp>
  <trd lng="br">unani&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>sdru&zcaron;it</trd>,
    <trd>sjednotit</trd>,
    <trd>slou&ccaron;it</trd>
  </trdgrp>
  <trdgrp lng="de">
        <trd>vereinigen</trd>,
        <trd>einen</trd>
     </trdgrp>
  <trd lng="fi">yhdist&auml;&auml;</trd>
  <trd lng="fr">unir</trd>
  <trd lng="he">&lamed;<ind>&alef;&het;&dalet;</ind></trd>
  <trd lng="hu">egyes&iacute;t</trd>
  <trdgrp lng="id">
        <trd>[<ind>satu</ind>] menyatukan</trd>,
        <trd>[<ind>satu</ind>] mempersatukan</trd>
      </trdgrp>
  <trd lng="it">unire</trd>
  <trdgrp lng="nl">
        <trd>verenigen</trd>,
        <trd>eenmaken</trd>
     </trdgrp>
  <trd lng="no">forene</trd>
  <trdgrp lng="ru">
        <trd>&c_o;&c_b;&c_malmol;&c_je;&c_d;&c_i;&c_n;&c_ja;&c_t;&c_mol;</trd>,
        <trd>&c_o;&c_b;&c_malmol;&c_je;&c_d;&c_i;&c_n;&c_i;&c_t;&c_mol;</trd>
     </trdgrp>
  <trdgrp lng="sk">
    <trd>sp&aacute;ja&tcaron;</trd>,
    <trd>stmeli&tcaron;</trd>,
    <trd>zjednoti&tcaron;</trd>,
    <trd>zl&uacute;&ccaron;i&tcaron;</trd>
  </trdgrp>
</drv>

    <drv mrk="unu.0igxi">
      <kap><tld/>i&gcirc;i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
        <dif>
          Kuni&gcirc;i en unu tuton:
          <ekz>
            la oksigeno <tld/>i&gcirc;as kun hidrogeno por naski akvon.
         </ekz>
       </dif>
        <trdgrp lng="ca">
          <trd>unir-se</trd>,
          <trd>ajuntar-se</trd>
       </trdgrp>
     </snc>
      <snc>
        <dif>
          Kuni&gcirc;i per politika a&ubreve; simila ligo:
          <ekz>
            proponi al &ccirc;iuj amikoj de interpopola justeco
            <tld/>i&gcirc;i en apartan
            grupon
            <fnt>
              <bib>Homaranismo</bib>
              <vrk>Homaranismo</vrk>
            </fnt>;
         </ekz><ekz>
            la Po&scirc;ta <tld/>i&gcirc;o;
         </ekz><ekz>
            tio alkondukos eble al la <ind><tld/>i&gcirc;o</ind> de la
            homaro
            <fnt>
              <bib>Homaranismo</bib>
              <vrk>Nefermita letero al s-ro de Beaufront.</vrk>
            </fnt>.
            <trdgrp lng="ca">
              <trd>uni&oacute;</trd>,
              <trd>associaci&oacute;</trd>
           </trdgrp>
           <trd 
lng="ru">&c_o;&c_b;&c_malmol;&c_je;&c_d;&c_i;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
         </ekz>
       </dif>
        <refgrp tip="vid">
          <ref cel="akord.0igxi">akordi&gcirc;i</ref>,
          <ref cel="asoci.0o">asocio</ref>,
          <ref cel="lig.0o">ligo</ref>,
          <ref cel="societ.0o">societo</ref>
       </refgrp>
        <trdgrp lng="ca">
          <trd>associar-se</trd>,
          <trd>aliar-se</trd>,
          <trd>coalitzar-se</trd>
       </trdgrp>
     </snc>
      <trdgrp lng="be">
        <trd>&c_a;&c_b;'&c_ja;&c_d;&c_n;&c_a;&c_c;&c_c;&c_a;</trd>,
        <trd>&c_z;&c_l;&c_u;&c_ch;&c_y;&c_c;&c_c;&c_a;</trd>
     </trdgrp>
      <trd lng="br">en em <ind>unani&ntilde;</ind></trd>
      <trdgrp lng="de">
        <trd>sich <ind>vereinigen</ind></trd>,
        <trd>zusammengehen</trd>
     </trdgrp>
      <trdgrp lng="es">
        <trd>unirse</trd>,
        <trd>juntarse</trd>
      </trdgrp>
      <trdgrp lng="fi">
        <trd>yhty&auml;</trd>,
        <trd>yhdisty&auml;</trd>
     </trdgrp>
      <trd lng="fr">s'<ind>unir</ind></trd>
      <trd lng="he">&lamed;&he;&tav;<ind>&alef;&het;&dalet;</ind></trd>
      <trd lng="hu">egyes&uuml;l</trd>
      <trd lng="id">[<ind>satu</ind>] bersatu</trd>
      <trd lng="it">unirsi</trd>
      <trd lng="nl">zich <ind>verenigen</ind></trd>
      <trd lng="no">forene seg</trd>
      <trdgrp lng="ru">
        <trd>&c_o;&c_b;&c_malmol;&c_je;&c_d;&c_i;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
        <trd>&c_o;&c_b;&c_malmol;&c_je;&c_d;&c_i;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
        <trd>&c_s;&c_o;&c_je;&c_d;&c_i;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
        <trd>&c_s;&c_o;&c_je;&c_d;&c_i;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
      </trdgrp>
      <trd lng="sv">f&ouml;renas</trd>
   </drv>

    <drv mrk="unu.0hava">
      <kap><tld/>hava</kap>
      <uzo tip="fak">MAT</uzo>
      <snc>
        <dif>
          <klr>(p.p. <ref cel="ring.0o.MAT">ringo</ref>)</klr>
          Posedanta <ref cel="unu.0o.neuxtra"><tld/>on</ref>.
       </dif>
     </snc>
      <trd lng="ca">unitari</trd>
      <trd lng="de">mit <ind>Einselement</ind></trd>
      <trd lng="en">with <ind>unit</ind></trd>
      <trd lng="fi">ykk&ouml;salkiollinen</trd>
      <trd lng="fr">unif&egrave;re</trd>
      <trd lng="hu">egys&eacute;gelemes</trd>
      <trd lng="it">unifero<klr> (mat.)</klr></trd>
      <trd lng="pl"><klr tip="ind">(pier&sacute;cie&nacute;) </klr>z
        <ind>jedynk&aogonek;</ind></trd>
      <trd lng="ru">&c_s; 
<ind>&c_je;&c_d;&c_i;&c_n;&c_i;&c_c;&c_je;&c_j;</ind><klr>
          (&c_k;&c_o;&c_l;&c_mol;&c_c;&c_o; &c_i;
          &c_t;.&c_p;.)</klr></trd>
   </drv>

    <drv mrk="unu.imaginara0o">
      <kap>imaginara <tld/>o</kap>
      <uzo tip="fak">MAT</uzo>
      <fnt><bib>MatVort</bib></fnt>
      <snc>
        <dif>
          La <ref cel="komple.0o.MAT">komplekso</ref>
          <frm am="i"><k>i</k></frm>.
       </dif>
     </snc>
  <trd lng="ca">unitat imagin&agrave;ria</trd>
  <trd lng="cs">imagin&aacute;rn&iacute; jednotka</trd>
  <trd lng="de">imagin&auml;re <ind>Einheit</ind></trd>
  <trd lng="en">imaginary <ind>unit</ind></trd>
  <trd lng="fi">imaginaariyksikk&ouml;</trd>
  <trd lng="fr">unit&eacute; imaginaire</trd>
  <trd lng="hu">k&eacute;pzetes <ind>egys&eacute;g</ind></trd>
  <trd lng="it"><ind>unit&agrave;</ind> immaginaria</trd>
  <trd lng="nl">imaginaire <ind>eenheid</ind></trd>
  <trdgrp lng="pl">
        <trd>jedno&sacute;&cacute; urojona</trd>,
        <trd>jednostka urojona</trd>
     </trdgrp>
  <trd lng="ru">&c_m;&c_n;&c_i;&c_m;&c_a;&c_ja;
        <ind>&c_je;&c_d;&c_i;&c_n;&c_i;&c_c;&c_a;</ind></trd>
  <trd lng="sk">imagin&aacute;rna jednotka</trd>
</drv>

    <drv mrk="unu.mal0eco">
      <kap>mal<tld/>eco</kap>
      <snc mrk="unu.mal0eco.KOMUNE">
        <dif>
          <ref tip="vid" cel="plur.0eco.KOMUNE">Plureco</ref>,
          diseco, malsameco de konsisto, de opinioj...:
          <ekz>
            <tld/>eco estas forteco kaj mal<tld/>eco estas malforteco
            <fnt>
              <aut>R. Corsetti</aut>,
              <vrk>Unueco estas forteco kaj malunueco estas
                malforteco!</vrk>,
              <lok>Spegulo, 2008:1</lok>
           </fnt>.
         </ekz>
       </dif>
        <refgrp tip="vid">
          <ref cel="bunt.0eco.FIG">bunteco<sncref/></ref>,
          <ref cel="hetero.0eco.KOMUNE">heterogeneco</ref>,
          <ref cel="konkor.mal0o.KOMUNE">malkonkordo</ref>
       </refgrp>
     </snc>
  <trdgrp lng="ca">
        <trd>diversitat</trd>,
        <trd>heterogene&iuml;tat</trd>,
        <trd>disparitat</trd>,
        <trd>mescla</trd>,
     </trdgrp>
  <trd lng="cs">rozkol</trd>
  <trdgrp lng="fr">
        <trd>disparit&eacute;</trd>,
        <trd>pluralit&eacute;</trd>
     </trdgrp>
  <trd lng="nl">onenigheid</trd>
  <trd lng="no">uenighet</trd>
  <trdgrp lng="sk">
    <trd>nejednotnos&tcaron;</trd>,
    <trd>nesvornos&tcaron;</trd>,
    <trd>rozkol</trd>
  </trdgrp>
</drv>

    <drv mrk="unu.mezur0o">
      <kap>mezur<tld/>o</kap>
      <snc mrk="unu.mezur0o.SCI">
        <uzo tip="fak">SCI</uzo>
        <dif>
          Difinita kvanto elektita por taksi per komparo samspecajn
          kvantojn, kalkulante, kiomoble ili enhavas tiun kvanton
          a&ubreve;
          frakcion da &gcirc;i:
          <ekz>
            la metro estas la universala mezur<tld/>o de la longo.
         </ekz>
       </dif>
     </snc>
  <trd lng="br">unanenn vuzulia&ntilde;</trd>
  <trdgrp lng="ca">
        <trd>unitat m&egrave;trica</trd>,
        <trd>unitat de mesura</trd>
     </trdgrp>
  <trd lng="cs">m&ecaron;rn&aacute; jednotka</trd>
  <trd lng="fi">mittayksikk&ouml;</trd>
  <trd lng="fr">unit&eacute; de mesure</trd>
  <trd lng="hu">m&eacute;rt&eacute;kegys&eacute;g</trd>
  <trd lng="it"><ind>unit&agrave;</ind> di misura</trd>
  <trd lng="nl">eenheid</trd>
  <trd lng="no">enhet</trd>
  <trd lng="pl">jednostka miary</trd>
  <trd lng="ru">&c_je;&c_d;&c_i;&c_n;&c_i;&c_c;&c_a; &c_i;&c_z;&c_m;&c_je;&c_r;&c_je;&c_n;&c_i;&c_ja;</trd>
  <trd lng="sk">mern&aacute; jednotka</trd>
</drv>

    <drv mrk="unu.mon0o">
      <kap>mon<tld/>o</kap>
      <snc mrk="unu.mon0o.EKON">
        <uzo tip="fak">EKON</uzo>
        <dif>
          <ref tip="super" cel="unu.mezur0o">Mezur<tld/>o</ref>,
          servanta por kalkuli
          <ref tip="malprt" cel="mon.0o">mono</ref>n.
       </dif>
     </snc>
  <lstref lst="voko:monunuoj"/>
  <trd lng="br">unanenn voneiz</trd>
  <trd lng="ca">unitat monet&agrave;ria</trd>
  <trd lng="cs">m&ecaron;nov&aacute; jednotka</trd>
  <trd lng="fi">rahayksikk&ouml;</trd>
  <trd lng="fr">unit&eacute; mon&eacute;taire</trd>
  <trd lng="hu">p&eacute;nzegys&eacute;g</trd>
  <trd lng="id"><ind>mata</ind> uang</trd>
  <trd lng="it"><ind>unit&agrave;</ind> monetaria</trd>
  <trd lng="nl">munteenheid</trd>
  <trd lng="no">myntenhet</trd>
  <trd lng="ru">&c_d;&c_je;&c_n;&c_je;&c_zh;&c_n;&c_a;&c_ja; &c_je;&c_d;&c_i;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">pe&ncaron;a&zcaron;n&aacute; jednotka</trd>
</drv>

    <drv mrk="unu.monasub0o">
      <kap>mona sub<tld/>o</kap>
      <snc mrk="unu.monasub0o.EKON">
        <uzo tip="fak">EKON</uzo>
        <dif>
          <ref tip="super" cel="unu.sub0o">Sub<tld/>o</ref> de
          <ref tip="malprt" cel="unu.mon0o.EKON">monunuo</ref>:
          <ekz>
            soldo estas malnova franca mona sub<tld/>o.
         </ekz>
       </dif>
     </snc>
      <trd lng="br">isunanenn voneiz</trd>
      <trd lng="ca">subunitat monet&agrave;ria</trd>
      <trd lng="fi">rahan alayksikk&ouml;</trd>
      <trd lng="fr">sous-unit&eacute; mon&eacute;taire</trd>
      <trd lng="hu">v&aacute;lt&oacute;p&eacute;nz</trd>
      <trdgrp lng="it">
        <trd><ind>sottounit&agrave;</ind> monetaria</trd>, 
        <trd>sottomultiplo<klr> (di unit&agrave; monetaria)</klr></trd>
      </trdgrp>
      <trd lng="ru">&c_r;&c_a;&c_z;&c_m;&c_je;&c_n;&c_n;&c_a;&c_ja; &c_m;&c_o;&c_n;&c_je;&c_t;&c_a;</trd>
   </drv>

    <drv mrk="unu.pliol0">
      <kap>pli ol <tld/></kap>
      <snc mrk="unu.pliol0.pluraj">
        <dif>
          <ref tip="dif" cel="plur.0aj">Pluraj</ref>:
          <ekz>
            pli ol <tld/> viro forlasis altan rangon por virino
            <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 14a</lok></fnt>.
          </ekz>
        </dif>
     </snc>
  <trd lng="ca">alguns</trd>
  <trd lng="cs">nejeden</trd>
  <trdgrp lng="fi">
        <trd>enemm&auml;n kuin yksi</trd>,
        <trd>usea</trd>
      </trdgrp>
  <trd lng="fr">plus d'un</trd>
  <trd lng="it"><ind>pi&ugrave;</ind> d'uno</trd>
  <trdgrp lng="ru">
        <trd>&c_b;&c_o;&c_l;&c_je;&c_je; &c_o;&c_d;&c_n;&c_o;&c_g;&c_o;</trd>,
        <trd>&c_n;&c_je;&c_s;&c_k;&c_o;&c_l;&c_mol;&c_k;&c_o;</trd>
      </trdgrp>
  <trd lng="sk">nejeden</trd>
</drv>

    <drv mrk="unu.sub0o">
      <kap>sub<tld/>o</kap>
      <snc mrk="unu.sub0o.SCI">
        <dif>
          Ono de
          <ref tip="malprt" cel="unu.mezur0o">mezur<tld/>o</ref>:
          <ekz>
            centimetro estas sub<tld/>o de metro;
         </ekz><ekz>
            metro estas sub<tld/>o de kilometro.
         </ekz>
       </dif>
     </snc>
      <trd lng="br">isunanenn</trd>
      <trd lng="ca">subunitat</trd>
      <trdgrp lng="fi">
        <trd>alayksikk&ouml;</trd>,
        <trd>kerrannaisyksikk&ouml; <klr tip="amb"> (yksik&ouml;n
            murto-osa)</klr></trd>
     </trdgrp>
      <trd lng="fr">sous-multiple<klr> (d'unit&eacute; de
      mesure)</klr></trd>
      <trd lng="hu">alegys&eacute;g</trd>
      <trd lng="it">sottomultiplo<klr> (di misura)</klr></trd>
      <trd lng="nl">onderdeel</trd>
      <trd lng="ru">&c_d;&c_o;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_je;&c_d;&c_i;&c_n;&c_i;&c_c;&c_a; &c_i;&c_z;&c_m;&c_je;&c_r;&c_je;&c_n;&c_i;&c_ja;</trd>
   </drv>

    <drv mrk="unu.super0o">
      <kap>super<tld/>o</kap>
      <snc mrk="unu.super0o.SCI">
        <dif>
          <ref tip="malprt" cel="unu.mezur0o">Mezur<tld/>o</ref>,
          kiu estas oblo de la baza:
          <ekz>
            kilometro estas super<tld/>o de metro.
         </ekz>
       </dif>
     </snc>
      <trd lng="ca">m&uacute;ltiple</trd>
      <trdgrp lng="fi">
        <trd>kerrannaisyksikk&ouml; <klr tip="amb"> (yksik&ouml;n
            monikerta)</klr></trd>
      </trdgrp>
      <trd lng="fr">multiple<klr> (d'unit&eacute; de mesure)</klr></trd>
      <trd lng="hu">t&ouml;bbsz&ouml;r&ouml;s egys&eacute;g</trd>
      <trd lng="it">multiplo<klr> (di misura)</klr></trd>
      <trd lng="ru">&c_k;&c_r;&c_a;&c_t;&c_n;&c_a;&c_ja; &c_je;&c_d;&c_i;&c_n;&c_i;&c_c;&c_a; &c_i;&c_z;&c_m;&c_je;&c_r;&c_je;&c_n;&c_i;&c_ja;</trd>
   </drv>

<drv mrk="unu.0_post_la_alia">
  <kap><tld/> post la alia,
  <var><kap><tld/> post alia</kap></var>,
  <var><kap><tld/> post <tld/></kap></var></kap>
  <snc mrk="unu.0_post_la_alia.KOMUNE">
    <dif>
      <ref tip="vid" cel="sekv.sin0a">Sinsekve</ref>,
      <ref tip="vid" cel="vic.laux0e">la&ubreve;vice</ref>,
      <tld/>ope la&ubreve; la tempo:
      <ekz>
        festoj kaj baloj sekvis <tld/> post la alia
        <fnt>
          <aut>-</aut>
          <vrk>La Karaj Braceletoj</vrk>
          <lok>en: Fundamenta Krestomatio de la lingvo Esperanto,
          1904</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiuj &ccirc;i vortoj estis kreataj <tld/> post <tld/>, la&ubreve; la
        kreskanta bezono
        <fnt>
          <aut>E. Privat</aut>
          <vrk>Historio de la lingvo Esperanto</vrk>
          <lok>1912</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">l'un apr&egrave;s l'autre</trd>
</drv>
