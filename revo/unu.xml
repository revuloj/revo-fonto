     </trdgrp>
  <trd lng="he">&resh;&alef;&shin;&yod;&tav;</trd>
  <trd lng="hu">el&odblac;sz&ouml;r <klr 
tip="ind">(sorrendben)</klr></trd>
  <trdgrp lng="it"><trd>innanzitutto</trd>, <trd>per
