<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: atropi.xml,v 1.19 2023/08/30 18:23:38 revo Exp $">
<kap>
  <rad>atropin</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="atropi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      <frm>C<sub>17</sub>H<sub>23</sub>O<sub>3</sub>N</frm>, alkaloido, tre venena,
      eltirata el <ref cel="belado.0o">beladono</ref>, kaj plilar&gcirc;iganta la
      pupilon:
      <ekz>
        <nom>Pierre Joseph Pelletier</nom> <klr>[&#x2026;]</klr> esploris kaj sintezis plurajn alkaloidojn
        same kiel atropinon (1831), strikninon (1818), kininon (1820) <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=524074">Pierre Joseph Pelletier</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_t;&c_r;&c_a;&c_p;&c_ib;&c_n;</trd>
  <trd lng="bg">&c_a;&c_t;&c_r;&c_o;&c_p;&c_i;&c_n;</trd>
  <trd lng="ca">atropina</trd>
  <trd lng="cs">atropin</trd>
  <trd lng="de">Atropin</trd>
  <trd lng="en">atropine</trd>
  <trd lng="es">atropina</trd>
  <trd lng="fr">atropine</trd>
  <trd lng="he">&#x5D0;&#x5D8;&#x5E8;&#x5D5;&#x5E4;&#x5D9;&#x5DF;</trd>
  <trd lng="hu">atropin</trd>
  <trd lng="it">atropina</trd>
  <trd lng="ja">&#x30A2;&#x30C8;&#x30ED;&#x30D4;&#x30F3;</trd>
  <trd lng="pl">atropina</trd>
  <trd lng="ru">&c_a;&c_t;&c_r;&c_o;&c_p;&c_i;&c_n;</trd>
  <trd lng="sk">atrop&iacute;n</trd>
</drv>
</art>
