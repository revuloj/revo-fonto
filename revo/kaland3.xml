<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <rad>kalandr</rad>/o
  </kap>
<drv mrk="kaland3.0o">
  <kap><tld/>o</kap>
    <uzo tip="fak">ZOO</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Mediteranea birdo (<trd lng="la">Melanocorypha calandra</trd>)
      el la familo <ref cel="alauxd1.0oj">ala&ubreve;dedoj</ref>, 
      kun bela kanto.
    </dif>
  </snc>
  <refgrp tip="hom">
    Ne miksu kun:
    <ref cel="kaland.0o">kalandro</ref> (grenskarabo),
    <ref cel="kaland1.0o">kalandro</ref> (ago kalandri).
  </refgrp>
  <trd lng="es">calandria</trd>
  <trd lng="fr">calandre<klr tip="ind"> (oiseau)</klr></trd>
</drv>
</art>

