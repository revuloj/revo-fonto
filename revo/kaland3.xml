<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kaland3.xml,v 1.13 2023/08/31 19:24:08 revo Exp $">
   <kap>
     <rad>kalandr</rad>/o
     <fnt><bib>PIV1</bib></fnt>
   </kap>
<drv mrk="kaland3.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
       Mediteranea birdo (<trd lng="la">Melanocorypha calandra</trd>)
       el la familo <ref cel="alauxd1.0oj">ala&ubreve;dedoj</ref>,
       kun bela kanto:
      <ekz>
        imitado, &ccirc;u de fonmediaj bruoj, &ccirc;u de apudaj birdoj, renkonti&gcirc;as en multaj
        birdokantoj, foje la birdo imitas la tutan kanton, pli ofte imitadoj
        estas enmetitaj  meze de la propra kanto (<tld/>a ala&ubreve;do) 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=51625">Birdokanto</url></lok>
        </fnt>.
      </ekz>
     </dif>
    <bld lok="&WCU;/thumb/0/07/Melanocorypha_calandra.jpg/545px-Melanocorypha_calandra.jpg" prm="CC BY-SA 3.0"><tld/>o (Melanocorypha calandra)
        <fnt>
          <aut>D&ucirc;rzan c&icirc;rano, Osado, Bere&scedil;e.jpg</aut>
          <lok><url ref="&WCW;/File:Melanocorypha_calandra.jpg">&WCW;/File:Melanocorypha_calandra.jpg</url></lok>
        </fnt>
    </bld>
   </snc>
   <refgrp tip="hom">
     Ne miksu kun:
     <ref cel="kaland.0o">kalandro</ref> (grenskarabo),
     <ref cel="kaland1.0o">kalandro</ref> (ago kalandri).
   </refgrp>
   <trd lng="de">Kalanderlerche</trd>
  <trd lng="es">calandria</trd>
   <trd lng="fi">arokiuru</trd>
   <trd lng="fr">calandre<klr tip="ind"> (oiseau)</klr></trd>
   <trd lng="ja">&#x30AF;&#x30ED;&#x30A8;&#x30EA;&#x30B3;&#x30A6;&#x30C6;&#x30F3;&#x30B7;</trd>
  <trdgrp lng="pt">
     <trd>calandra <klr tip="ind">(zool.)</klr></trd>,
     <trd>calhandra <klr tip="ind">(zool.)</klr></trd>
   </trdgrp>
   <trd lng="nl">kalanderleeuwerik</trd>
   <trdgrp lng="pl">
     <trd>kalandra szara</trd>,
     <trd>skowronek <ind>kalandra</ind></trd>
   </trdgrp>
   <trd lng="ru">&c_s;&c_t;&c_je;&c_p;&c_n;&c_o;&c_j; &c_zh;&c_a;&c_v;&c_o;&c_r;&c_o;&c_n;&c_o;&c_k;</trd>
  <trd lng="uk">&#x441;&#x442;&#x435;&#x43F;&#x43E;&#x432;&#x438;&#x439; &#x436;&#x430;&#x439;&#x432;&#x43E;&#x440;&#x43E;&#x43D;&#x43E;&#x43A; &#x441;&#x43F;&#x456;&#x432;&#x443;&#x447;&#x438;&#x439;</trd>
</drv>
</art>

