<drv mrk="krur.0umo">
  <kap><tld/>umo</kap>
  <snc mrk="krur.0umo.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      &Ccirc;irka&ubreve;<tld/>a parto de
      <ref tip="malprt" cel="pantal.0o.TEKS">pantalono</ref>
      a&ubreve; de <ref tip="malprt" cel="bot.0o.TEKS">boto</ref>;
      <ref tip="sin" cel="tub.pantalon0o.TEKS">pantalontubo</ref>:
      <ekz>
        li vi&scirc;is tran&ccirc;ilon je sia pantalona <tld/>umo
        <fnt>
          <aut>P. Haanp&auml;&auml;, trad. B. Lehtinen</aut>,
          <vrk><url ref="http://esperanto-ondo.ru/Tl-finn2.htm">
          Baraka artisto</url></vrk>,
          <lok>La Ondo de Esperanto, 2002:12 (98)</lok>
        </fnt>;
      </ekz>
      <ekz>
        orfe staris sur la planko lia sola piedo apud la dua <tld/>umo
        <fnt>
          <vrk><url ref="http://miresperanto.com/biblioteko/jamburg/17.htm">17 -
          miresperanto.com</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">nohavice</trd>
  <trd lng="de">Hosenbein</trd>
  <trd lng="en">pant leg</trd>
  <trd lng="es">pernera</trd>
  <trd lng="fr">jambe<klr> (de v&ecirc;tement)</klr></trd>
  <trd lng="sk">nohavica</trd>
</drv>

<drv mrk="krur.0ringo">
  <kap><tld/>ringo</kap>
  <fnt><bib>PV</bib><lok>braceleto</lok></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="bracel.0o">Braceleto</ref> portata &ccirc;e
      la <tld/>o:
      <ekz>
        <tld/>ringo <klr>[&#8230;]</klr> por rajdistoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=34564">Ringo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>esclava</trd>,
    <trd>canyellera, turmellera</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ankle bracelet</trd>,
    <trd>anklet</trd>, <trd>leg bangle</trd>
  </trdgrp>
  <trd lng="fr">bracelet<klr tip="ind"> (de cheville)</klr></trd>
  <trd lng="nl">enkelband</trd>
  <trd lng="pl">bransoleta</trd>
</drv>
