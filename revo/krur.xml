<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>krur</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="krur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="krur.0o.genuo_gxis_piedo">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Parto de suba membro de homo, ekde genuo &gcirc;is piedo.
    </dif>
    <trd lng="be">&c_g;&c_a;&c_l;&c_jo;&c_n;&c_k;&c_a;</trd>
    <trd lng="ca">cama</trd>
    <trd lng="de">Unterschenkel</trd>
    <trdgrp lng="en">
      <trd>lower leg</trd>,
      <trd>shank</trd>,
      <trd>crus</trd>
    </trdgrp>
    <trd lng="es">pierna<klr> (entre rodilla y pie)</klr></trd>
    <trd lng="fr">tibia</trd>
    <trd lng="nl">onderbeen</trd>
    <trd lng="pl">gole&nacute;</trd>
    <trdgrp lng="pt">
     <trd>perna <klr tip="amb">(entre o joelho e o
     tornozelo)</klr></trd>,
     <trd>canela <klr tip="amb">(anat.)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_g;&c_o;&c_l;&c_je;&c_n;&c_mol;</trd>
    <trd lng="sv">underben</trd>
  </snc>
  <snc mrk="krur.0o.gambo">
    <dif>
      Tuto de la suba membro de homo a&ubreve; besto:
      <ekz>
        veturi sur sia paro da <tld/>oj
        <klr>(piediri)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        unu fidas la <tld/>ojn de sia &ccirc;evalo, alia siajn
        proprajn <tld/>ojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la hundoj regalis sin per <tld/>oj kaj flugiloj de kokidoj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="femur.0o">femuro</ref>,
      <ref cel="gamb.0o">gambo</ref>,
      <ref cel="pied.0o">piedo</ref>.
    </refgrp>
    <trd lng="be">&c_n;&c_a;&c_g;&c_a;</trd>
    <trd lng="bg">&c_k;&c_r;&c_a;&c_k;</trd>
    <trdgrp lng="ca">
      <trd>cama</trd>,
      <trd>pota</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>been</trd>,<trd>poot</trd>
    </trdgrp>
    <trd lng="de">Bein</trd>
    <trd lng="en">leg</trd>
    <trd lng="es">pierna</trd>
    <trd lng="fr">jambe</trd>
    <trd lng="pl">noga</trd>
    <trd lng="ru">&c_n;&c_o;&c_g;&c_a;</trd>
    <trd lng="sv">ben</trd>
  </snc>
  <snc mrk="krur.0o.mebla">
    <dif>
      Simila parto de objekto;
      <ref tip="sin" cel="pied.0o.mebla">piedo<sncref/></ref>:
      <ekz>
        la du <tld/>oj de cirkelo<fnt>PIV</fnt>;
      </ekz>
      <ekz>
        &scirc;i penegis grimpi unu el la <tld/>oj de la tablo,
        sed &gcirc;i estis tro glitiga
        <fnt>
        <aut>L. Carroll, trad. D. Broadribb</aut>,
        <vrk>Alico en Mirlando</vrk>,
        <bib>LOdE</bib>, <lok>1999:10 (60), &ccirc;ap. 1a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la infanoj ne povis sidi sur la se&gcirc;oj, &ccirc;ar
        &ccirc;iuj ties <tld/>oj estis
        rompitaj
        <fnt>
        <aut>B. Wennergren</aut>,
        <vrk>PMEG</vrk>,
        <lok><url
        ref="http://purl.oclc.org/net/pmego?d=pmeg/pmeg9/diqpli.htm">Pli
        ol unu difinilo</url></lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_n;&c_o;&c_zh;&c_k;&c_a;</trd>
    <trdgrp lng="ca">
      <trd>petge</trd>,
      <trd>pota</trd>,
      <trd>peu</trd>      
    </trdgrp>
    <trdgrp lng="de">
      <trd>Bein</trd>,
      <trd>Fu&szlig;</trd>,
      <trd>Schenkel</trd>
    </trdgrp>
    <trd lng="nl">poot</trd>
    <trd lng="en">leg</trd>
    <trd lng="es">pata</trd>
    <trd lng="fr">pied<klr tip="ind"> (d'un objet)</klr></trd>
    <trd lng="pl">noga</trd>
    <trd lng="ru">&c_n;&c_o;&c_zh;&c_k;&c_a;</trd>
    <trd lng="sv">ben</trd>
  </snc>
  <trdgrp lng="hu">
    <trd>l&aacute;bsz&aacute;r</trd>,
    <trd>l&aacute;b <klr tip="amb"> (&aacute;tv.. is)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>gamba</trd>,
    <trd>zampa</trd>
  </trdgrp>
  <trd lng="oc">camba</trd>
</drv>

<drv mrk="krur.0umo">
  <kap><tld/>umo</kap>
  <snc mrk="krur.0umo.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      &Ccirc;irka&ubreve;<tld/>a parto de
      <ref tip="malprt" cel="pantal.0o.TEKS">pantalono</ref>
      a&ubreve; de <ref tip="malprt" cel="bot.0o.TEKS">boto</ref>;
      <ref tip="sin" cel="tub.pantalon0o.TEKS">pantalontubo</ref>:
      <ekz>
        li vi&scirc;is tran&ccirc;ilon je sia pantalona <tld/>umo
        <fnt>
          <aut>P. Haanp&auml;&auml;, trad. B. Lehtinen</aut>,
          <vrk><url ref="http://esperanto-ondo.ru/Tl-finn2.htm">
          Baraka artisto</url></vrk>,
          <lok>La Ondo de Esperanto, 2002:12 (98)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">jambe<klr> (de v&ecirc;tement)</klr></trd>
</drv>

<drv mrk="krur.0ringo">
  <kap><tld/>ringo</kap>
  <fnt><bib>PV</bib><lok>braceleto</lok></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="bracel.0o">Braceleto</ref> portata &ccirc;e la
      <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>esclava</trd>,
    <trd>canyellera, turmellera</trd>
  </trdgrp>
  <trd lng="fr">bracelet<klr tip="ind"> (de cheville)</klr></trd>
  <trd lng="nl">enkelband</trd>
  <trd lng="pl">bransoleta</trd>
</drv>
</art>
