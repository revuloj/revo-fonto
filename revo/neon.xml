<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>neon</rad>/o
</kap>
<drv mrk="neon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 10, atompezo 20,179, simbolo Ne. Malofta <ref tip="super"
      cel="gas.nobla0o">nobla gaso</ref> (heliano), trovi&gcirc;anta en la aero
      kaj uzata por reklama lumigado.
    </dif>
  </snc>
  <trd lng="de">Neon</trd>
  <trd lng="en">neon</trd>
  <trd lng="hu">neon</trd>
  <trd lng="nl">neon</trd>
  <trd lng="ru">&c_n;&c_je;&c_o;&c_n;</trd>
</drv>
</art>
