<art mrk="$Id: bauxd.xml,v 1.12 2021/03/04 19:18:49 revo Exp $">
<kap>
  <rad>ba&ubreve;d</rad>/o
</kap>
<drv mrk="bauxd.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">TEL</uzo>
  <mlg>Bd</mlg>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:mezurunuoj" cel="unu.mezur0o.SCI">Mezurunuo</ref>
      de la rapido de elektronika datentranssendo, egala al
      unu signal&scirc;an&gcirc;o sekunde:
      <ekz>
        MLT-3 havas kodigan efikecon 1 bito/<tld/>o, tamen &gcirc;i postulas
        kvar transirojn
        (<tld/>ojn) por kompletigi plenan ciklon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=511554">MLT-3</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        50 <tld/>oj, t.e. komerca rapideco de Telekso
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=244349">Ba&ubreve;do</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_o;&c_d;</trd>
    <trd lng="ca">baud</trd>
    <trd lng="de">Baud</trd>
    <trd lng="nl">baud</trd>
    <trd lng="en">baud</trd>
    <trd lng="es">baudio</trd>
    <trd lng="fa">&a_b;&a_damma_w;&a_d;</trd>
    <trd lng="fr">baud</trd>
    <trd lng="hu">baud</trd>
    <trd lng="pl">bod</trd>
    <trd lng="pt">baud</trd>
    <trd lng="ru">&c_b;&c_o;&c_d;</trd>
    <trd lng="sk">baud</trd>
  </snc>
  <trdgrp lng="he">
    <trd>&#x5D1;&#x5D0;&#x5D5;&#x5D3;</trd>,
    <trd>&#x5E7;&#x5E6;&#x5D1; &#x5E9;&#x5D9;&#x5D3;&#x5D5;&#x5E8;</trd>
  </trdgrp>
</drv>
</art>

