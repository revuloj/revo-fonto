<drv mrk="penetr.ne0ebla">
  <kap>ne<tld/>ebla, <var><kap>netra<tld/>ebla</kap></var></kap>
  <snc mrk="penetr.ne0ebla.KOMUNE">
    <dif>
      Prezentanta netraireblan baron:
      <ekz>
        la mallumo estis netra<tld/>ebla
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ne<tld/>ebla viza&gcirc;o;
      </ekz>
      <ekz>
        tute ne<tld/>ebla arbaro
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la dornoplenaj lianoj prezentas netra<tld/>eblan densejon
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        sin levis netra<tld/>ebla, silenta polva uragano
        <fnt>
          <bib>Lasta</bib>
          <lok>La Lasta</lok>
        </fnt>;
      </ekz>
      <ekz>
        netra<tld/>eblaj nebuloj
        <fnt>
          <bib>Lasta</bib>
          <lok>La Lasta</lok>
        </fnt>;
      </ekz>
      <ekz>
        fine &ccirc;io malaperis en la griza, movi&gcirc;ema, netra<tld/>ebla abismo
        <fnt>
          <bib>Lasta</bib>
          <lok>La Lasta</lok>
        </fnt>;
      </ekz>
      <ekz>        
        estas ne<tld/>ebla la Dia justeco
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dens.0a.multnombre">densa</ref>,
      <ref cel="ferm.0ita.PSI">fermita<sncref/></ref>,
      <ref cel="mister.0a">mistera</ref>,
      <ref cel="sond.ne0ebla">nesondebla</ref>,
      <ref cel="opak.0a.malluma">opaka<sncref/></ref>,
      <ref cel="vual.0i">vualita</ref>
    </refgrp>
    <refgrp tip="ant">
      <ref cel="las.tra0ema">tralasema</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_p;&c_r;&c_a;&c_n;&c_ib;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
