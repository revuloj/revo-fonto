<drv mrk="penetr.tra0i">
  <kap>tra<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="penetr.tra0i.tute">
    <dif>
      Tute, plene <tld/>i:
      <ekz>
        la glavon <klr>[&#x2026;]</klr> tra<tld/>is la postan parton de la korpo
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 3:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        tuta li <klr>[&#x2026;]</klr> estis diafana, kvaza&ubreve; tra<tld/>ita per
        sunradioj
        <fnt><bib>Metrop</bib></fnt>;
      </ekz>
      <ekz>
        la sonado de la pre&gcirc;ejaj sonoriloj tra<tld/>as malsupren al &scirc;i
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la feli&ccirc;o <klr>[&#x2026;]</klr> tra<tld/>as mian sangon, tra<tld/>as miajn pensojn
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la princo stre&ccirc;is la vidadon kaj a&ubreve;dadon, penante tra<tld/>i la polvan muron
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        stranga tremo tra<tld/>is min kaj senmovigis
        <fnt>
          <bib>FdO</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ir.tra0i.KOMUNE">trairi<sncref/></ref>,
      <ref cel="kur.tra0i">trakuri</ref>
    </refgrp>
  </snc>
  <snc mrk="penetr.tra0i.spione">
    <uzo tip="stl">FIG</uzo>
    <dif>
      El<ref cel="spion.0i">spioni</ref>, <ref cel="sond.0i.FIG">sondi</ref>:
      <ekz>
        la perfidulo <klr>[&#x2026;]</klr> penas tra<tld/>i la plej altan sekreton de la &scirc;tato
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_n;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_n;&c_ib;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>prolnout</trd>,
    <trd>pronikat</trd>,
    <trd>proniknout</trd>,
    <trd>vnikat</trd>,
    <trd>vniknout</trd>
  </trdgrp>
  <trd lng="de">durchdringen</trd>
  <trd lng="fr">percer<klr tip="amb"> (p&eacute;n&eacute;trer)</klr></trd>
  <trdgrp lng="hu">
    <trd>&aacute;thatol</trd>,
    <trd>&aacute;that</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3057;&#x307F;&#x3068;&#x304A;&#x308B;</trd>,
    <trd>&#x7A81;&#x304D;&#x901A;&#x3059;  [<ind>&#x3064;&#x304D;&#x3068;&#x304A;&#x3059;</ind>]</trd>,
    <trd>&#x8CAB;&#x901A;&#x3059;&#x308B;  [<ind>&#x304B;&#x3093;&#x3064;&#x3046;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">binnendringen</trd>
  <trdgrp lng="pl">
    <trd>penetrowa&cacute; si&lstroke;&aogonek;</trd>,
    <trd>przenika&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>p&abreve;trunde cu for&#x21B;a</trd>,
    <trd>strecura</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_n;&c_i;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>predra&tcaron; sa</trd>,
    <trd>prenika&tcaron;</trd>,
    <trd>presakova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_o;&c_n;&c_i;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_r;&c_i;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_r;&c_i;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja; &c_n;&c_a;&c_s;&c_k;&c_r;&c_ib;&c_z;&c_mol;</trd>
  </trdgrp>
</drv>
