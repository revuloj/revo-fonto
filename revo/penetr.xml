<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: penetr.xml,v 1.24 2023/09/03 12:45:06 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>penetr</rad>/i
</kap>
<drv mrk="penetr.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc mrk="penetr.0i.enigxi">
    <gra><vspec>x</vspec></gra>
    <dif>
      <ref tip="super" cel="en.0igxi">Eni&gcirc;i</ref> en ion, trairante la
      eblajn barojn:
      <ekz>
        la &scirc;tono <tld/>is en lian frunton, kaj li falis
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 17:49</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiuj tri herooj trarompe <tld/>is en la tendaron
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 23:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        viaj sagoj <klr>[&#x2026;]</klr> <tld/>os en la koron de la malamikoj
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 45:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        vortoj de kalumnianto estas kiel franda&jcirc;oj, kaj 
        ili <tld/>as en la profundon de la ventro
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 18:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu vorto, kiun &scirc;i diris, <tld/>is en mian koron kiel tran&ccirc;ilo
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        lo&gcirc;i profunde sub la tero, kien la lumo de la suno neniam <tld/>as
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; unu radio de la suno ne povis <tld/>i tra la
        densaj <klr>[&#x2026;]</klr> bran&ccirc;oj de la arboj
        <fnt><bib>Fab1</bib>, <lok>Sova&gcirc;aj cignoj</lok></fnt>;
      </ekz>
      <ekz>
        subite <tld/>is al li agrabla odoro de pomoj
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la rigardo de la knabineto <tld/>is rekte en la &ccirc;ambron
        <fnt>
          <bib>Fab2</bib>
          <lok>Knabineto kun alumetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>is &gcirc;is la fino de la arbaro
        <fnt>
          <bib>Fab2</bib>
          <lok>Sonorilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        akvo <klr>[&#x2026;]</klr> <tld/>as &ccirc;ien
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj vestoj, <tld/>itaj per peza akvo, &scirc;in
        malsupren tiris
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="merg.0i">mergi</ref>,
      <ref cel="sond.0i">sondi</ref>,
      <ref cel="tremp.0i">trempi</ref>
    </refgrp>
  </snc>
  <snc mrk="penetr.0i.PSI">
    <gra><vspec>tr</vspec></gra>
    <uzo tip="klr">(pri sentoj)</uzo>
    <dif>
      Plene <ref cel="posed.ek0i.okupi">ekposedi</ref>, 
      emocii, <ref cel="absorb.0i">absorbi</ref>:
      <ekz>
        teruro &scirc;in <tld/>is &gcirc;is la profundo de la brusto
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i sentis sin kvaza&ubreve; <tld/>ita de &gcirc;oja sendan&gcirc;ereco
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kanto <klr>[&#x2026;]</klr> tiel profunde <tld/>is en lian animon, 
        <klr>[kvaza&ubreve;]</klr> li mem estas &ccirc;i tiu ama kanto
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>os vin la spirito de la Eternulo
        <fnt><bib>MT</bib>, <lok>&Sa1; 10:6</lok></fnt>;
      </ekz>
      <ekz>
        li metis sur sin malbenon, kiel veston, kaj &gcirc;i <tld/>is en lian internon, kiel
        akvo, kaj en liajn ostojn, kiel oleo
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 109:18</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hant.0i">hanti</ref>,
      <ref cel="kapt.0i.infekti">kapti<sncref/></ref>,
      <ref cel="reg.0i.FIG_iun">regi<sncref/></ref>
    </refgrp>
  <trd lng="de">erfassen <klr>(Gef&uuml;hl)</klr></trd>
  </snc>
  <snc mrk="penetr.0i.malkovri">
    <dif>
      <ref tip="super" cel="kovr.mal0i">Malkovri</ref>, <ref cel="diven.0i">diveni</ref>:
      <ekz>
        mia koro <tld/>is multon da sa&gcirc;o kaj scio
        <fnt>
          <bib>MT</bib>
          <lok>Predikanto 1:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        la plej turmenta stre&ccirc;i&gcirc;o de la volo ne povis fari tion, ke menso neinstruita
        <tld/>u per unu fojo la misterojn de la scienco
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ir.en0i">eniri</ref>,
      <ref cel="profun">enprofundi&gcirc;i</ref>,
      <ref cel="kompre.0i.havi_gxustan_ideon">kompreni<sncref/></ref>,
<ref cel="rivel.0i.KOM">riveli<sncref/></ref>
    </refgrp>
  <trd lng="de">erfassen <klr>(Wissen)</klr></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_n;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_n;&c_ib;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>prolnout</trd>,
    <trd>pronikat</trd>,
    <trd>proniknout</trd>,
    <trd>vnikat</trd>,
    <trd>vniknout</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>durchdringen</trd>,
    <trd>eindringen</trd>,
    <trd>dringen</trd>,
    <trd>penetrieren</trd>
  </trdgrp>
  <trd lng="en">penetrate</trd>
  <trd lng="fr">p&eacute;n&eacute;trer</trd>
  <trdgrp lng="hu">
    <trd>behatol</trd>,
    <trd>&aacute;that</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5165;&#x308A;&#x8FBC;&#x3080;  [<ind>&#x306F;&#x3044;&#x308A;&#x3053;&#x3080;</ind>]</trd>,
    <trd>&#x4FB5;&#x5165;&#x3059;&#x308B;  [<ind>&#x3057;&#x3093;&#x306B;&#x3085;&#x3046;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x8CAB;&#x304F;  [<ind>&#x3064;&#x3089;&#x306C;&#x304F;</ind>]</trd>,
    <trd>&#x6D78;&#x900F;&#x3059;&#x308B;  [<ind>&#x3057;&#x3093;&#x3068;&#x3046;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x3057;&#x307F;&#x8FBC;&#x3080;  [<ind>&#x3057;&#x307F;&#x3053;&#x3080;</ind>]</trd>,
    <trd>&#x898B;&#x629C;&#x304F;  [<ind>&#x307F;&#x306C;&#x304F;</ind>]</trd>,
    <trd>&#x5BDF;&#x3059;&#x308B;  [<ind>&#x3055;&#x3063;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>doordringen</trd>,
    <trd>penetreren</trd>,
    <trd>binnendringen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>penetrowa&cacute;</trd>,
    <trd>nurtowa&cacute;</trd>,
    <trd>wnika&cacute;</trd>,
    <trd>zag&lstroke;&eogonek;bia&cacute;</trd>
  </trdgrp>
  <trd lng="pt">penetrar</trd>
  <trdgrp lng="ro">
    <trd>penetra</trd>,
    <trd>intra</trd>,
    <trd>p&abreve;trunde</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_n;&c_i;&c_k;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_n;&c_i;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>penetrova&tcaron;</trd>,
    <trd>prenika&tcaron;</trd>,
    <trd>prestupova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_o;&c_n;&c_i;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_d;&c_o;&c_h;&c_o;&c_d;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_r;&c_i;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_u;&c_v;&c_ib;&c_h;&c_o;&c_d;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="penetr.0a">
  <kap><tld/>a</kap>
  <snc mrk="penetr.0a.enigxa">
    <dif>
      Facile kaj profunde <tld/>anta:
      <ekz>
        <nom>Chilo</nom>, sidinte dum ia tempo sur la sojlo, eksentis <tld/>an malvarmon
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        sinjorino, kiu havis akran, <tld/>an vo&ccirc;on
        <fnt>
          <bib>KrM</bib>
          <lok>20. Kontra&ubreve; &Ccirc;iu Malfacilo Ekzistas Konsilo.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> starigas <tld/>ajn demandojn
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan MacGill</aut>
          <vrk><url ref="&Monato;/2005/008712.php">Dudirekta instruo kaj influo</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        spertoj povas esti tiel <tld/>aj, ke ili glui&gcirc;as dum la tuta vivo
        <fnt>
          <bib>Monato</bib>
          <aut>Petro de Smedt</aut>
          <vrk><url ref="&Monato;/2010/009946.php">La verde farbita birdo</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_r;&c_a;&c_n;&c_ib;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
    <trdgrp lng="de">
      <trd>durchdringend</trd>,
      <trd>eindringend</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_n;&c_i;&c_k;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>,
      <trd>&c_v;&c_s;&c_je;&c_p;&c_r;&c_o;&c_n;&c_i;&c_k;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="penetr.0a.PSI">
    <dif>
      <ref cel="vid.akra0a">Akravida</ref>, rimarkanta kaj komprenanta ka&scirc;ita&jcirc;on:
      <ekz>
        lia rigardo <klr>[&#x2026;]</klr> estis <tld/>a, kvaza&ubreve; &gcirc;i volus eni&gcirc;i &gcirc;is &scirc;ia interno
        <fnt>
          <bib>HsT</bib>
          <lok>Heredanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la esploranta, <tld/>a rigardo moli&gcirc;is
        <fnt>
          <bib>HsT</bib>
          <lok>Heredanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pastrino <tld/>e lin rigardis
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 19a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_r;&c_a;&c_n;&c_ib;&c_k;&c_l;&c_ib;&c_v;&c_y;</trd>
    <trdgrp lng="de">
      <trd>durchbohrend</trd>,
      <trd>stechend <klr>(Blick)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_n;&c_i;&c_c;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trd lng="cs">pronikav&yacute;</trd>
  <trd lng="fr">p&eacute;n&eacute;trant</trd>
  <trdgrp lng="hu">
    <trd>behat&oacute;</trd>,
    <trd>&aacute;that&oacute;</trd>,
    <trd>penetr&aacute;ns</trd>
  </trdgrp>
  <trd lng="nl">doordringend</trd>
  <trdgrp lng="pl">
    <trd>przenikaj&aogonek;cy</trd>,
    <trd>wnikliwy</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>penetrant</trd>,
    <trd>p&abreve;trunz&abreve;tor</trd>
  </trdgrp>
  <trd lng="sk">prenikav&yacute;</trd>
</drv>

<drv mrk="penetr.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="penetr.0ema.PSI">
    <dif>
      Volonte, facile <tld/>anta ka&scirc;ita&jcirc;ojn:
      <ekz>
        la &ccirc;asisto <klr>[&#x2026;]</klr> vidis &ccirc;ion per siaj <tld/>emaj
        okuloj
        <fnt><bib>ElektFab</bib>, <lok>sesope oni trairas la tutan
        mondon</lok></fnt>.
      </ekz>
      <ekz>
        lin komprenas nur kelkaj dekoj da kapoj tiel <tld/>emaj, kiel li
        <fnt><bib>FK</bib>, <lok>4, rakontoj, nokto, (de Eube el
        Odeso)</lok></fnt>;
      </ekz>
      <ekz>
        respondis la figuro per tiel <tld/>ema vo&ccirc;o, ke &gcirc;i trapikis lian koron, kiel
        fenicia ponardo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        la mastrino, <tld/>ema kaj vigla, tuj kaptis la momenton de embarasi&gcirc;o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        via <tld/>ema intelekto scios ekkoni la veron
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LXXIII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">pronikav&yacute;</trd>
  <trdgrp lng="de">
    <trd>durchdringlich</trd>,
    <trd>eindringlich</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>incisif<klr tip="amb"> (clairvoyant)</klr></trd>,
    <trd>p&eacute;n&eacute;trant</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6D78;&#x900F;&#x529B;&#x306E;&#x3042;&#x308B;  [<ind>&#x3057;&#x3093;&#x3068;&#x3046;&#x3061;&#x304B;&#x3089;&#x306E;&#x3042;&#x308B;</ind>]</trd>,
    <trd>&#x3057;&#x307F;&#x901A;&#x308B;  [<ind>&#x3057;&#x307F;&#x3068;&#x304A;&#x308B;</ind>]</trd>,
    <trd>&#x92ED;&#x3044;  [<ind>&#x3059;&#x308B;&#x3069;&#x3044;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">doordringend</trd>
  <trdgrp lng="pl">
    <trd>przenikliwy</trd>,
    <trd>badawczy</trd>
  </trdgrp>
  <trd lng="ro">penetrant</trd>
  <trd lng="sk">prenikav&yacute;</trd>
  <trd lng="uk">&c_p;&c_r;&c_o;&c_n;&c_i;&c_k;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>
</drv>

<drv mrk="penetr.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="penetr.0igi.enigi">
    <dif>
      Igi iun a&ubreve; ion <tld/>i en:
      <ekz>
        <tld/>igu &ccirc;i tiujn vortojn en viajn orelojn
        <fnt><bib>NT</bib>, <lok>&Luk; 9:44</lok></fnt>;
      </ekz>
      <ekz>
        kiam vi estos disi&gcirc;ontaj, <tld/>igu la tran&ccirc;ilon en arbon &ccirc;e la vojkruco
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la unukornulo <klr>[&#x2026;]</klr> <tld/>igis tiel forte la kornon en la trunkon, ke
        &gcirc;i ne povis eltiri &gcirc;in
        <fnt>
          <bib>ElektFab</bib>
          <lok>La kura&gcirc;a tajloreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la nuna momento, plene okupita de turmenta maltrankvileco, <tld/>igis ankora&ubreve;
        nedifinitan sed jam mordantan maldol&ccirc;econ en la koron de la virino
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Marta</nom> profunde <tld/>igis sian brilantan rigardon en la viza&gcirc;on de la anta&ubreve; &scirc;i
        staranta homo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        konsternita kalkulisto <klr>[&#x2026;]</klr> <tld/>igis sian nazon en la paperojn
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igi en la vivon la uzadon de esperanto
        <fnt><bib>Lanti</bib>, <lok>ne&ubreve;tralismo &ndash;
        ne&ubreve;traluloj</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pusx.en0i">enpu&scirc;i</ref>,
      <ref cel="sxov.en0i">en&scirc;ovi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_k;&c_a;&c_r;&c_a;&c_n;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_k;&c_a;&c_r;&c_e;&c_n;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>prosadit</trd>,
    <trd>prostr&ccaron;it</trd>,
    <trd>protla&ccaron;it</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>hinein <ind>sto&szlig;en</ind></trd>,
    <trd>hinein <ind>schieben</ind></trd>
  </trdgrp>
  <trd lng="fr">faire <ind>p&eacute;n&eacute;trer</ind></trd>
  <trd lng="hu">bejuttat</trd>
  <trd lng="ja">&#x5165;&#x308A;&#x8FBC;&#x307E;&#x305B;&#x308B;  [<ind>&#x306F;&#x3044;&#x308A;&#x3053;&#x307E;&#x305B;&#x308B;</ind>]</trd>
  <trd lng="nl">doen <ind>binnendringen</ind></trd>
  <trd lng="pl">przenikni&eogonek;cie powodowa&cacute;</trd>
  <trd lng="ro">p&abreve;trunde</trd>
  <trd lng="ru">&c_v;&c_n;&c_je;&c_d;&c_r;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>presadi&tcaron;</trd>,
    <trd>prestr&ccaron;i&tcaron;</trd>,
    <trd>pretla&ccaron;i&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="penetr.en0i">
  <kap>en<tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="penetr.en0i.KOMUNE">
    <dif>
      Profunde <tld/>i:
      <ekz>
        la &ccirc;ambro <klr>[&#x2026;]</klr> estas tiel aran&gcirc;ita, ke la tutan
        pluvon, kiu povus en<tld/>i, &gcirc;i forkondukas
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj, la lo&gcirc;ejoj
        de la termitoj</lok></fnt>;
      </ekz>
      <ekz>
        iom da fre&scirc;a aero <klr>[&#x2026;]</klr> en<tld/>is en la &ccirc;ambrojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la aromon de la realeco, kiu en<tld/>as en la menson kaj penson de la rigardanto
        kaj restas tie por &ccirc;iam, ili ne povas transdoni
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        en nian kabanon en<tld/>is soldatoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        forta kaj bela kverko <klr>[kies]</klr> radikoj en<tld/>as profunde kaj fortike
        en la teron
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        sova&gcirc;a decido en<tld/>as lian animon
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekkvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        en<tld/>i en la misteron, kiu necesigas &ccirc;i tiun artifikon
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vera signifo de la letero plene en<tld/>is lian cerbon
        <fnt>
          <bib>TAA</bib>
          <lok>&Ccirc;apitro VI.</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_n;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_n;&c_ib;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>
