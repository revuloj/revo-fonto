<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: trol1.xml,v 1.18 2023/10/21 14:37:42 revo Exp $">
<kap>
  <rad>trol</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="trol1.0o">
  <kap><tld/>o</kap>
  <snc mrk="trol1.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Granda konusa <ref tip="super" cel="ret.fisx0o.CXAS">fi&scirc;reto</ref>, kiun oni
      tiras tra akvo a&ubreve; sur fundo de akvejo:
      <ekz>
        protekti sova&gcirc;ajn bestojn kontra&ubreve; tiuj metodoj de &ccirc;asado,
        kaptado kaj <tld/>o-fi&scirc;ado
        <fnt>
          <vrk><url ref="http://eo.wikisource.org/wiki/La_Charto_de_la_Tero">
          La &Ccirc;arto de la Tero</url></vrk>,
          <lok>2005-06</lok>
        </fnt>
      </ekz>
    </dif>
    <rim mrk="trol1.0o.RIM_HOM">
      <ref tip="hom" cel="trol.0o.MIT">Trolo</ref> anka&ubreve; estas
      norde&ubreve;ropa feo.
    </rim>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_a;&c_l;</trd>
  <trdgrp lng="ca">
    <trd>art de r&ograve;ssec</trd>,
    <trd>rossegall</trd>
  </trdgrp>
  <trd lng="cs">vle&ccaron;n&aacute; s&iacute;&tcaron;</trd>
  <trd lng="de">Schleppnetz</trd>
  <trd lng="en">trawl</trd>
  <trd lng="es">red de arrastre<klr tip="amb"> (pesca)</klr></trd>
  <trd lng="fr">chalut</trd>
  <trd lng="hu">fen&eacute;kvon&oacute;h&aacute;l&oacute;</trd>
  <trd lng="it">sciabica<klr> (pesca)</klr></trd>
  <trd lng="ja">&#x30C8;&#x30ED;&#x30EB;</trd>
  <trdgrp lng="nl">
    <trd>trawlnet</trd>,
    <trd>sleepnet</trd>
  </trdgrp>
  <trd lng="pl">tra&lstroke;</trd>
  <trdgrp lng="pt">
    <trd>arrast&atilde;o <klr tip="amb">(rede de pesca)</klr></trd>,
    <trd>rede de arrasto</trd>
  </trdgrp>
  <trd lng="sk">vle&ccaron;n&aacute; sie&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x440;&#x438;&#x431;&#x430;&#x43B;&#x44C;&#x441;&#x44C;&#x43A;&#x438;&#x439; &#x442;&#x440;&#x430;&#x43B;</trd>,
    <trd>&#x442;&#x440;&#x43E;&#x43B;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="trol1.0sxipo">
  <kap><tld/>&scirc;ipo</kap>
  <snc mrk="trol1.0sxipo.MAR">
  <uzo tip="fak">MAR</uzo>
    <dif>
      &Scirc;ipo, ekipita por fi&scirc;kaptado per <tld/>o:
      <ekz>
        ni ekvidis, tre fore, malgrandan <tld/>&scirc;ipon, kiu anta&ubreve;eniris,
        nerimarkeble, sur la brileganta maro
      <fnt>
        <aut>Albert Camus, trad. Michel Duc Goninaz</aut>,
        <vrk>La Fremdulo</vrk>,
        <lok>S.A.T., 1993, p. 63</lok>
      </fnt>;
      </ekz>
      <ekz>
        en&scirc;ipi&gcirc;inta sur <tld/>&scirc;ipo en Paimpol, li tuj ali&gcirc;is al la Liberaj Francaj Fortoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=264823">Jacques P&acirc;ris de
          Bollardi&egrave;re</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_r;&c_a;&c_w;&c_l;&c_je;&c_r;</trd>
    <trd lng="de">Trawler</trd>
    <trd lng="en">trawler</trd>
    <trd lng="fr">chalutier</trd>
  </snc>
  <trd lng="ja">&#x30C8;&#x30ED;&#x30FC;&#x30EB;&#x8239;  <pr>&#x30C8;&#x30ED;&#x30FC;&#x30EB;&#x305B;&#x3093;</pr></trd>
  <trdgrp lng="uk">
    <trd>&#x442;&#x440;&#x430;&#x443;&#x43B;&#x435;&#x440;</trd>,
    <trd>&#x440;&#x438;&#x431;&#x43E;&#x43B;&#x43E;&#x432;&#x43D;&#x435; &#x441;&#x443;&#x434;&#x43D;&#x43E;</trd>
  </trdgrp>
</drv>
</art>
