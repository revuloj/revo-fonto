<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: psalte.xml,v 1.12 2015/10/02 06:10:18 revo Exp $">
<kap>
  <rad>psalter</rad>/o
</kap>
<drv mrk="psalte.0o">
  <kap><tld/>o</kap>
  <snc mrk="psalte.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Antikva hebrea <ref tip="lst" cel="instru2.kord0o.MUZ"
      lst="voko:kordinstrumentoj">kordinstrumento</ref>, speco de <ref
      tip="vid"
      cel="citr.0o.MUZ">citro</ref>:
      <ekz>
        sur dekkorda <tld/>o mi muzikos al Vi
        <fnt><bib>MT</bib>, <lok>&Psa; 144:9</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">psaltery</trd>
  <trd lng="de">Psalter</trd>
  <trd lng="fr">psalt&eacute;rion</trd>
  <trdgrp lng="hu">
    <trd>psalterium</trd>,
    <trd>lapcitera</trd>,
    <trd>keretcitera</trd>
  </trdgrp>
  <trd lng="nl">psalter</trd>
  <trdgrp lng="pl">
    <trd>psalterium</trd>,
    <trd>psalterion</trd>
  </trdgrp>
  <trd lng="pt">salt&eacute;rio</trd>
</drv>
</art>
