<art mrk="$Id: psalte.xml,v 1.16 2024/06/25 08:31:46 revo Exp $">
<kap>
  <rad>psalter</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="psalte.0o">
  <kap><tld/>o</kap>
  <snc mrk="psalte.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Antikva hebrea <ref tip="lst" cel="instru2.kord0o.MUZ"
      lst="voko:kordinstrumentoj">kordinstrumento</ref>, speco de <ref tip="vid"
      cel="citr.0o.MUZ">citro</ref>:
      <ekz>
        sur dekkorda <tld/>o mi muzikos al Vi
        <fnt><bib>MT</bib>, <lok>&Psa; 144:9</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="citar.0o">citaro</ref>,
      <ref cel="harp.0o">harpo</ref>
    </refgrp>
    <bld
    lok="&WCU;/thumb/2/20/Gorleston_Psalter_psaltery_player.png/222px-Gorleston_Psalter_psaltery_player.png"
    prm="PD">ludanto de <tld/>o &ccirc; 1310&#x202F;-&#x202F;1326
        <fnt>
          <aut>Unknown authorUnknown author, File:F107v_b.png</aut>
          <lok><url
          ref="&WCW;/File:Gorleston_Psalter_psaltery_player.png">https://commons.wikimedia.org/wiki/File:Gorleston_Psalter_psaltery_player.png</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_s;&c_a;&c_l;&c_t;&c_y;&c_r; <klr>(&c_ib;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_e;&c_n;&c_t;)</klr></trd>,
    <trd>&c_p;&c_s;&c_a;&c_l;&c_t;&c_e;&c_r;&c_y;&c_jo;&c_n;</trd>
  </trdgrp>
  <trd lng="en">psaltery</trd>
  <trd lng="de">Psalter<klr>(ium)</klr></trd>
  <trd lng="fr">psalt&eacute;rion</trd>
  <trdgrp lng="hu">
    <trd>psalterium</trd>,
    <trd>lapcitera</trd>,
    <trd>keretcitera</trd>
  </trdgrp>
  <trd lng="ja">&#x30D7;&#x30B5;&#x30EB;&#x30C6;&#x30EA;&#x30A6;&#x30E0;</trd>
  <trd lng="nl">psalter</trd>
  <trdgrp lng="pl">
    <trd>psalterium</trd>,
    <trd>psalterion</trd>
  </trdgrp>
  <trd lng="pt">salt&eacute;rio</trd>
</drv>
</art>
