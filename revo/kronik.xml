<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kronik.xml,v 1.29 2025/04/01 15:31:44 revo Exp $">
<kap>
  <rad>kronik</rad>/a <fnt><bib>PVEF</bib></fnt>,
  <var><kap><rad var="&hcirc;">&hcirc;ronik</rad>/a <fnt><bib>PVEF</bib></fnt></kap></var>
</kap>
<drv mrk="kronik.0a">
  <kap><tld/>a, <var><kap><tld var="&hcirc;"/>a</kap></var></kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <uzo tip="klr">(pp malsano)</uzo>
    <dif>
      Longe <ref tip="super" cel="dauxr.0a.dauxranta">da&ubreve;ra</ref>:
      <ekz>
        alkoholo mem povas ka&ubreve;zi diversajn malsanojn, ekzemple
        kanceron kaj <tld/>an hepatiton<fnt>
        <aut>Peng Juan</aut>:
        <vrk><url ref="&Monato;/2001/007379.html">Alkohola&jcirc;o &ccirc;iam dan&gcirc;era</url></vrk>,
        <bib>Monato</bib>
        <lok>2001/04, p. 22</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> duone apatia mieno montris homon <tld var="&hcirc;"/>e malsanan
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        periodo de <tld/>a dubo, suspekto kaj malmemfido
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2003/007983.html">Lingvo kiel politikilo</url></vrk>
          <bib>Monato</bib>
          <lok>2003/07, p. 6</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="akut.0a.malsano">akuta<sncref/></ref>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: (en la senco de longeda&#365;ra) prefere diru <ke>longeda&#365;ra</ke>.
  </rim>

  <refgrp tip="hom">
    <ref cel="kronik1.0a">kronika</ref>
  </refgrp>
  <trd lng="be">&c_h;&c_r;&c_a;&c_n;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">cr&ograve;nic</trd>
  <trdgrp lng="cs">
    <trd>chronick&yacute;</trd>,
    <trd>st&aacute;l&yacute;</trd>,
    <trd>trval&yacute;</trd>,
    <trd>vlekl&yacute;</trd>
  </trdgrp>
  <trd lng="de">chronisch</trd>
  <trdgrp lng="en">
    <trd>chronic</trd>,
    <trd>inveterate</trd>,
    <trd>lingering</trd>
  </trdgrp>
  <trd lng="es">cr&oacute;nico</trd>
  <trdgrp lng="fr">
    <trd>chronique<klr> (maladie)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>id&uuml;lt</trd>,
    <trd>kr&oacute;nikus</trd>
  </trdgrp>
  <trd lng="ja">&#24930;&#24615;&#12398;  <pr>&#12414;&#12435;&#12379;&#12356;&#12398;</pr></trd>
  <trdgrp lng="nl">
    <trd>langdurig</trd>,
    <trd>chronisch</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>chroniczny</trd>,
    <trd>przewlek&lstroke;y</trd>
  </trdgrp>
  <trd lng="pt">cr&ocirc;nico</trd>
  <trd lng="ro">cronic</trd>
  <trd lng="ru">
  &c_h;&c_r;&c_o;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>chronick&yacute;</trd>,
    <trd>zastaran&yacute;</trd>,
    <trd>zd&lacute;hav&yacute;</trd>
  </trdgrp>
  <trd lng="sv">kronisk</trd>
  <trdgrp lng="uk">
    <trd>&#1083;&c_ib;&c_t;&#1086;&#1087;&c_i;&c_s;&#1085;&c_i;&c_j;</trd>,
    <trd>&c_h;&c_r;&#1086;&#1085;&c_ib;&#1082;&c_a;&#1083;&#1100;&#1085;&c_i;&c_j;</trd>,
    <trd>&c_h;&c_r;&#1086;&#1085;&c_ib;&c_ch;&#1085;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#24930;&#24615; <pr>m&#224;nx&#236;ng</pr></trd>,
    <trd>&#38263;&#26399;&#22320; <pr>ch&#225;ngq&#299;de</pr></trd>,
    <trd>&#38271;&#26399;&#22320; <pr>ch&#225;ngq&#299;de</pr></trd>,
    <trd>&#28040;&#32791;&#24615; <pr>xi&#257;oh&#224;ox&#236;ng</pr></trd>,
    <trd>&#20064;&#24815;&#24615;&#22320; <pr>x&#237;gu&#224;nx&#236;ngde</pr></trd>,
    <trd>&#32722;&#24931;&#24615;&#22320; <pr>x&#237;gu&#224;nx&#236;ngde</pr></trd>
  </trdgrp>
</drv>
</art>
