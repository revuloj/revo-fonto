<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kronik</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="kronik.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <uzo tip="klr">(pp malsano)</uzo>
    <dif>
      Longeda&ubreve;ra:
      <ekz>
        alkoholo mem povas ka&ubreve;zi diversajn malsanojn, ekzemple kanceron
        kaj <tld/>an hepatiton<fnt><aut>Peng Juan</aut>: <vrk>Alkohola&jcirc;o
        &ccirc;iam dan&gcirc;era</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 4a,
        p. 22a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="hom">
      <ref cel="kronik1.0a">kronika</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_h;&c_r;&c_a;&c_n;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">cr&ograve;nic</trd>
  <trd lng="de">chronisch</trd>
  <trdgrp lng="en">
    <trd>chronic</trd>,
    <trd>inveterate</trd>,
    <trd>lingering</trd>
  </trdgrp>
  <trd lng="es">cr&oacute;nico</trd>
  <trdgrp lng="fr">
    <trd>chronique<klr> (maladie)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>id&uuml;lt</trd>,
    <trd>kr&oacute;nikus</trd>
  </trdgrp>
  <trd lng="ru">&c_h;&c_r;&c_o;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sv">kronisk</trd>
</drv>
</art>
