<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>rubl</rad>/o
</kap>
<drv mrk="rubl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="lst" cel="unu.mon0o" lst="voko:monunuoj">Monunuo</ref>
      de Rusujo kaj 
      Belarusujo, pasintece - de Sovetunio, centoblo de
      <ref tip="prt" cel="kopek.0o">kopeko</ref>:
      <ekz>
        &leftquot;nur dek na&ubreve; <tld/>ojn mi elspezas en
monato&rightquot;, li skribis
        <fnt><bib>VivZam</bib> <lok>&ccirc;apitro 4a, p. 39a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_u;&c_b;&c_je;&c_l;&c_mol;</trd>
  <trd lng="de">Rubel</trd>
  <trd lng="en">ruble</trd>
  <trdgrp lng="fr">
    <trd>rouble</trd>
  </trdgrp>
  <trd lng="hu">rubel</trd>
  <trd lng="id">rubel</trd>
  <trd lng="nl">roebel</trd>
  <trd lng="pl">rubel</trd>
  <trd lng="pt">rublo</trd>
  <trd lng="ru">&c_r;&c_u;&c_b;&c_l;&c_mol;</trd>
</drv>
</art>
