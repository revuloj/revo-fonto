<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: feromon.xml,v 1.14 2023/08/31 13:11:28 revo Exp $">
<kap>
  <rad>feromon</rad>/o <fnt><bib>PIV2</bib></fnt> 
</kap>
<drv mrk="feromon.0o">
  <kap><tld/>o</kap>
  <snc mrk="feromon.0o.KEM">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia substanco ellasata de vivulo kaj peranta
      informojn al samspeciaj vivuloj, kiuj flaras &gcirc;in:
      <ekz>
        oni distingas seksajn, alarmajn,
        grupigajn  ktp. <tld/>ojn,
        depende de ilia pli specifa rolo
      <fnt>
        <aut>L. Kov&aacute;cs</aut>,
        <vrk><url ref="&STEB;biologio/feromonoj/feromonoj.htm">
          Pri la feromonoj</url></vrk>,
          <lok>Scienca Revuo, 1991, (42), p. 54a-60a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_e;&c_r;&c_a;&c_m;&c_o;&c_n;</trd>
  <trd lng="bg">&c_f;&c_je;&c_r;&c_o;&c_m;&c_o;&c_n;</trd>
  <trd lng="ca">feromona</trd>
  <trd lng="cs">feromon</trd>
  <trd lng="de">Pheromon</trd>
  <trd lng="en">pheromone</trd>
  <trd lng="fr">ph&eacute;romone</trd>
  <trd lng="it">feromone</trd>
  <trd lng="ja">&#x30D5;&#x30A7;&#x30ED;&#x30E2;&#x30F3;  [<ind>&#x3075;&#x3047;&#x308D;&#x3082;&#x3093;</ind>]</trd>
  <trdgrp lng="pt">
    <trd>feromona</trd>,
    <trd>ferom&ocirc;nio</trd>
  </trdgrp>
  <trd lng="sk">ferom&#xF3;n</trd>
  <trd lng="uk">&c_f;&c_je;&c_r;&#x43E;&#x43C;&#x43E;&#x43D;</trd>
</drv>
</art>

