<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dali.xml,v 1.25 2022/03/15 15:50:54 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>dali</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="dali.0o">
  <kap><tld/>o</kap>
  <snc mrk="dali.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Planto kun grandaj brilkoloraj floraroj,
      ofte kultivata &gcirc;ardene;
      genro el familio <ref tip="super" cel="astera.0oj">asteracoj</ref>
      (<trd lng="la">Dahlia</trd>);
      <ref tip="sin" cel="georgi.0o.BOT">georgino</ref>:
      <ekz>
        &scirc;i rigardis la pali&gcirc;antajn kolorojn de la restantaj
        ses-sep <tld/>oj en la bedo sub la fenestro
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_r;&c_g;&c_ib;&c_n;&c_ja;</trd>
  <trd lng="ca">d&agrave;lia</trd>
  <trdgrp lng="cs">
    <trd>georg&iacute;na</trd>,
    <trd>ji&rcaron;inka</trd>,
    <trd>ji&rcaron;ina</trd>,
    <trd>ji&rcaron;inka</trd>
  </trdgrp>
  <trd lng="de">Dahlie</trd>
  <trd lng="en">dahlia</trd>
  <trd lng="es">dalia</trd>
  <trd lng="fr">dahlia</trd>
  <trd lng="he">&#x5D3;&#x5DC;&#x5D9;&#x5D4;</trd>
  <trd lng="hu">d&aacute;lia</trd>
  <trd lng="nl">dahlia</trd>
  <trdgrp lng="pl">
    <trd>dalia</trd>,
    <trd>georginia</trd>
  </trdgrp>
  <trd lng="pt">d&aacute;lia</trd>
  <trd lng="ro">dalie</trd>
  <trd lng="ru">&c_g;&c_je;&c_o;&c_r;&c_g;&c_i;&c_n;</trd>
  <trd lng="sk">georg&iacute;na</trd>
  <trd lng="uk">&#x436;&#x43E;&#x440;&#x436;&#x438;&#x43D;&#x430;</trd>
</drv>
</art>
