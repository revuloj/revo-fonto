<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Meksiki</rad>/o<fnt>Z</fnt>
    </kap>

    <drv mrk="meksiki.M0o">
      <kap><tld/>o  <fnt><bib>PIV1</bib></fnt></kap>
      <uzo tip="fak">GEOG</uzo>
      <uzo tip="fak">POL</uzo>
      <snc>
        <ref tip="dif" cel="meksik.M0lando">Meksiklando</ref>
        <ekz>
          <klr>...</klr> plumserpento Ketzalkoatl en <tld/>o,
          falko de la Horusa templo &ccirc;e Edfu &#8208; bronza,
          ar&gcirc;enta a&ubreve; ora fa&ubreve;no svarmis en la kultejoj
          <fnt>
            <aut>G. Waringhien</aut>, <vrk>Ni kaj &Gcirc;i</vrk>.
            <lok>La Laguna: Stafeto, 1972. p. 46a</lok>
          </fnt>;
        </ekz><ekz>
          en <tld/>o oni povas kalkuli 80 naciecojn
          <fnt>
            <aut>E. Lanti</aut>:
            <vrk><url ref="http://home.arcor.de/gmickle/skk/92ms_2.html"
                >Sennaciismo</url></vrk>
          </fnt>;
        </ekz><ekz>
          la revolucio venkis, <tld/>o deklaris sian sendependecon
          <fnt><aut>Szathm&aacute;ri</aut>: <vrk>La guarbo</vrk></fnt>;
        </ekz><ekz>
          la penetro de variolo en <tld/>on en 1520 mortigis
          3,5 milionojn da homoj
          <fnt>
            <aut>V. Lemelev</aut>: <vrk>Homo, kiu savis milionojn</vrk>.
            <bib>Monato</bib>
          </fnt>;
        </ekz><ekz>
          koncerne la nutra&jcirc;ojn, <tld/>o ne estas memsufi&ccirc;a
          <fnt>
            <bib>Po&scirc;Atlas</bib>. <lok>p. 35&ndash;36</lok>
          </fnt>.
        </ekz>
      </snc>
      <trd lng="bg">&c_M;&c_je;&c_k;&c_s;&c_i;&c_k;&c_o;</trd>
      <trdgrp lng="ca">
        <trd>M&egrave;xic</trd>,
        <trd>Estats Units Mexicans</trd>
      </trdgrp>
      <trd lng="de">Mexiko</trd>
      <trd lng="en">Mexico</trd>
      <trd lng="fr">Mexique</trd>
      <trdgrp lng="es">
        <trd>M&eacute;xico</trd>,
        <trd>M&eacute;jico</trd>,
        <trd>Estados Unidos Mexicanos</trd>
      </trdgrp>
      <trd lng="nl">Mexico</trd>
      <trdgrp lng="pl">
        <trd>Meksyk</trd>,
        <trd>Meksyka&nacute;skie Stany Zjednoczone</trd>
      </trdgrp>
      <trd lng="ru">&c_M;&c_je;&c_k;&c_s;&c_i;&c_k;&c_a;</trd>
    </drv>

    <drv mrk="meksiki.0a">
      <kap><tld lit="m"/>a</kap>
      <snc>
        <dif>
          Rilata al <tld/>o:
          <ekz>
            danke al la iniciato de prof. Feldmann-Gonzales jam okazis
            multe
            atentata preleg-serio en pluraj <tld lit="m"/>aj
            universitatoj
            <fnt>
              <aut>Reinhard F&ouml;ssmeier</aut>: <vrk>AIS en Meksikio</vrk>.
              <bib>LOdE</bib>, <lok>N-ro 54 (1999:4)</lok>
            </fnt>;
          </ekz><ekz>
            Mono: <tld lit="m"/>a peso = 100 centavoj
            <fnt>
              <bib>Po&scirc;Atlas</bib>. <lok>p. 35&ndash;36</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="ca">mexic&agrave;<klr> (adj.)</klr></trd>
      <trd lng="en">Mexican</trd>
      <trd lng="nl">Mexicaans</trd>
      <trd lng="pl">meksyka&nacute;ski</trd>
      <trd lng="ru">&c_m;&c_je;&c_k;&c_s;&c_i;&c_k;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
    </drv>

    <drv mrk="meksiki.0ano">
      <kap><tld lit="m"/>ano</kap>
      <snc>
        <dif>
          Civitano a&ubreve; enlo&gcirc;anto de <tld/>o:
          <ekz>
            la <tld lit="m"/>a aviadilo atingos &Scirc;anhajon en la 5-a
            de majo por retransporti <tld lit="m"/>ajn pasa&gcirc;erojn
            kaj aviadilistojn de la kurso kaj aliajn <tld
              lit="m"/>anojn en &Ccirc;inio
            <fnt>
              <vrk><url
 ref="http://www.elerno.cn/bbs/dispbbs.asp?boardid=16&amp;id=8203"
                  >&Ccirc;inaj rimedoj por apartigi meksikiajn
                  turistojn estas ta&ubreve;gaj</url></vrk>
              <lok>2009-05-06</lok>
            </fnt>;
          </ekz><ekz>
            &ccirc;eestis <klr>...</klr> germanoj kaj <tld lit="m"/>anoj,
            poloj kaj pakistananoj
            <fnt>
              <vrk>La unua merkreda &gcirc;enerala audienco de Benedikto la
              16a</vrk>.
              <lok><url
              ref="http://www.ikue.org/papoj/benediktoxvi/prediko050427.html"
                  >IKUE, 27.04.2005</url></lok>
            </fnt>;
          </ekz><ekz>
            <ctl>La <tld lit="m"/>ano</ctl>
            <klr>(Rakonto de Jack London, tr. de E.Grobe, eld.
              Tempe: Arizona stelo, 2003)</klr>.
          </ekz>
        </dif>
      </snc>
      <trd lng="ca">mexic&agrave;<klr> (subst.)</klr></trd>
      <trd lng="nl">Mexicaan</trd>
      <trd lng="pl">Meksykanin</trd>
      <trd lng="ru">&c_m;&c_je;&c_k;&c_s;&c_i;&c_k;&c_a;&c_n;&c_je;&c_c;</trd>
    </drv>

    <drv mrk="meksiki.NovM0o">
      <kap>Nov-<tld/>o <fnt><bib>PIV1</bib></fnt>,
        <var>
          <kap>Nov<tld lit="m"/>o</kap>
        </var>
      </kap>
      <uzo tip="fak">GEOG</uzo>
      <uzo tip="fak">POL</uzo>
      <snc>
        <dif>
          Unu el la sudaj sub&scirc;tatoj de
          <ref tip="malprt" cel="uson.U0o">Usono</ref>:
          <ekz>
            radioobservoj &ccirc;e la Tre Granda Teleskoparo en
            Nov<tld lit="m"/>o poste identigis alian jetoduopon,
            kiu devenas de la kerno
            <fnt>
              <aut>Don Harlow</aut>:
              <vrk>Klarigo de misteraj spiralbrakoj?</vrk>
              <lok><url
 ref="http://www.harlows.org/don/astronomio/archives/000428.php"
                  >Astronomio</url>, 2007-04-13</lok>
            </fnt>;
          </ekz><ekz>
            <ctl>chicano</ctl> <klr>[estas]</klr> radikalismo en la
            kamparoj de Nov-<tld/>o kaj Kalifornio
            <fnt>
              <vrk>Usono, versio &leftquot;latina&rightquot;</vrk>.
              <lok><url ref="http://eo.mondediplo.com/article1102.html"
                  >&leftquot;Le Monde diplomatique&rightquot; en Esperanto.
                  2006-04-01</url>
              </lok>.
            </fnt>
          </ekz>
        </dif>
      </snc>
      <trd lng="ca">Nou <ind>M&egrave;xic</ind></trd>
      <trd lng="en">New Mexico</trd>
      <trd lng="nl">Nieuw-Mexico</trd>
      <trd lng="pl">Nowy Meksyk</trd>
      <trd lng="ru">&c_N;&c_mol;&c_ju;-&c_M;&c_je;&c_k;&c_s;&c_i;&c_k;&c_o;</trd>
    </drv>

  </art>

