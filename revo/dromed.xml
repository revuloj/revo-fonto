<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>dromedar</rad>/o
</kap>
<drv mrk="dromed.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="kamel.0o">kamelo</ref>
      (<trd lng="la">Camelus dromedarius</trd>), kun unu &gcirc;ibo.  
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_a;&c_m;&c_a;&c_d;&c_e;&c_r;</trd>
  <trd lng="de">Dromedar</trd>
  <trd lng="es">dromedario</trd>
  <trd lng="fr">dromadaire</trd>
  <trdgrp lng="hu">
    <trd>dromed&aacute;r</trd>,
    <trd>egyp&uacute;p&uacute; <ind>teve</ind></trd></trdgrp>
  <trd lng="nl">dromedaris</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_r;&c_o;&c_m;&c_a;&c_d;&c_je;&c_r;</trd>,
    <trd>&c_o;&c_d;&c_n;&c_o;&c_g;&c_o;&c_r;&c_b;&c_y;&c_j; &c_v;&c_je;&c_r;&c_b;&c_l;&c_ju;&c_d;</trd>
  </trdgrp>
  <trd lng="sv">dromedar</trd>
</drv>
</art>
