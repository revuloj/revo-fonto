<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dromed.xml,v 1.25 2023/11/04 18:50:52 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>dromedar</rad>/o
</kap>
<drv mrk="dromed.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="dromed.0o.ZOO">
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_specioj" cel="speci.0o.BIO">Specio</ref>
      de <ref tip="super" cel="kamel.0o">kamelo</ref>
      (<trd lng="la">Camelus dromedarius</trd>), kun unu &gcirc;ibo:
      <ekz>
        timigite staris la <tld/>o kaj la komercisto tiris al si la kaftanon super la
        kapon
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        sova&gcirc;aj <tld/>oj nun vivas nur en A&ubreve;stralio, la tieaj &ccirc;irka&ubreve; 500&#x202F;000 <tld/>oj
        devenas de malsova&gcirc;igitaj bestoj, kiujn oni alportis inter 1840 kaj 1907
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=115863">Dromedaro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="baktri.b0akamelo.ZOO">baktria kamelo</ref>
    <bld lok="&WCU;/thumb/b/bb/Camelus_dromedarius_in_Singapore_Zoo.JPG/640px-Camelus_dromedarius_in_Singapore_Zoo.JPG" prm="PD"><tld/>o en la besto&gcirc;ardeno de Singapuro
        <fnt>
          <aut>Daderot, propra verko</aut>
          <lok><url ref="&WCW;/File:Camelus_dromedarius_in_Singapore_Zoo.JPG">&WCW;/File:Camelus_dromedarius_in_Singapore_Zoo.JPG</url></lok>
        </fnt>
    </bld>
    <trd lng="be">&c_d;&c_r;&c_a;&c_m;&c_a;&c_d;&c_e;&c_r;</trd>
    <trdgrp lng="ca">
      <trd><ind>camell</ind> com&uacute;</trd>,
      <trd>dromedari</trd>
    </trdgrp>
    <trd lng="de">Dromedar</trd>
    <trd lng="en">dromedary</trd>
    <trd lng="es">dromedario</trd>
    <trd lng="fr">dromadaire</trd>
    <trdgrp lng="hu">
      <trd>dromed&aacute;r</trd>,
      <trd>egyp&uacute;p&uacute; <ind>teve</ind></trd></trdgrp>
    <trd lng="nl">dromedaris</trd>
    <trdgrp lng="pl">
      <trd>dromader</trd>,
      <trd>wielb&lstroke;&aogonek;d jednogarbny</trd>,
      <trd>jednogarb <klr>(rzadkie)</klr></trd>,
      <trd>dromedar <klr>(rzadkie)</klr></trd>
    </trdgrp>
    <trd lng="pt">dromed&aacute;rio</trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_r;&c_o;&c_m;&c_a;&c_d;&c_je;&c_r;</trd>,
      <trd>&c_o;&c_d;&c_n;&c_o;&c_g;&c_o;&c_r;&c_b;&c_y;&c_j;
      &c_v;&c_je;&c_r;&c_b;&c_l;&c_ju;&c_d;</trd>
    </trdgrp>
    <trd lng="sv">dromedar</trd>
    <trd lng="zh">&#x5355;&#x5CF0;&#x9A86;&#x9A7C; <pr>d&#x101;nf&#x113;nglu&#xF2;tu&#xF3;</pr></trd>
</snc>
  <trdgrp lng="cs">
    <trd>dromed&aacute;r</trd>,
    <trd>dromed&aacute;&rcaron;</trd>,
    <trd>velbloud jednohrb&yacute;</trd>
  </trdgrp>
  <trd lng="ja">&#x30D2;&#x30C8;&#x30B3;&#x30D6;&#x30E9;&#x30AF;&#x30C0;</trd>
  <trdgrp lng="sk">
    <trd>dromed&aacute;r</trd>,
    <trd>&tcaron;ava jednohrb&aacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x434;&#x440;&#x43E;&#x43C;&#x430;&#x434;&#x435;&#x440;</trd>,
    <trd>&#x434;&#x440;&#x43E;&#x43C;&#x435;&#x434;&#x430;&#x440;</trd>,
    <trd>&#x43E;&#x434;&#x43D;&#x43E;&#x433;&#x43E;&#x440;&#x431;&#x438;&#x439; &#x432;&#x435;&#x440;&#x431;&#x43B;&#x44E;&#x434;</trd>
  </trdgrp>
</drv>
</art>
