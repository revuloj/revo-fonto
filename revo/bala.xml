<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>bala</rad>/i
</kap>
<drv mrk="bala.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="bala.0i.malpurajxon">
    <dif>
      Forpu&scirc;i la polvon a&ubreve; malpura&jcirc;on per speciala ilo,
      konsistanta el fasko da vergetoj a&ubreve; haregoj:
      <ekz>
        se &ccirc;iu <tld/>os anta&ubreve; sia pordo, tiam en la tuta urbo
        estos ordo<fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="meti.0o">metio</ref>
    </refgrp>
  </snc>
  <trd lng="hu">seper</trd>
  <snc mrk="bala.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <ref tip="dif" cel="bala.for0i.FIG">for<tld/>i</ref>
    <ekz>
      la venkinta partio <tld/>is siajn
      kontra&ubreve;antojn.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_je;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_m;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">skuba&ntilde;</trd>
  <trd lng="cs">m&eacute;st</trd>
  <trdgrp lng="de">
    <trd>kehren</trd>,
    <trd>fegen</trd>
  </trdgrp>
  <trd lng="en">sweep</trd>
  <trd lng="es">barrer</trd>
  <trd lng="fr">balayer</trd>
  <trd lng="nl">vegen</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_m;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">sopa</trd>
  <trd lng="tr">s&uuml;p&uuml;rmek</trd>
</drv>

<drv mrk="bala.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="bala.0ajxo.balaitajxo">
    <dif>
      <tld/>ita&jcirc;o.
    </dif>
    <trdgrp lng="br">
      <trd>skubadur</trd>,
      <trd>skubien</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>balayure</trd>,
      <trd>ordure</trd>
    </trdgrp>
    <trd lng="nl">veegsel</trd>
    <trd lng="en">sweepings</trd>
  </snc>
  <snc mrk="bala.0ajxo.senvalorajxo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senvalora&jcirc;o.
    </dif>
    <refgrp tip="vid">
      <ref cel="bagate.0o">bagatelo</ref>,
      <ref cel="balast.0o">balasto</ref>,
      <ref cel="jxet">for&jcirc;eta&jcirc;o</ref>
      <ref cel="rub.0o">rubo</ref>
    </refgrp>
    <trd lng="fr">rebut</trd>
    <trdgrp lng="nl">
      <trd>afval</trd>,
      <trd>vuilnis</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_m;&c_je;&c_c;&c_mol;&c_c;&c_je;</trd>
  <trd lng="de">Kehricht</trd>
  <trd lng="hu">szem&eacute;t</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_r;</trd>,
    <trd>&c_m;&c_u;&c_s;&c_o;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="bala.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Ilo por <tld/>i:
      <ekz>
        sidi kiel muso sub <tld/>ilo
        <klr>(silenti, senmovi pro timo a&ubreve; singardo)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ja;&c_t;&c_l;&c_a;</trd>,
    <trd>&c_v;&c_je;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>balaenn</trd>,
    <trd>skubellenn</trd>
  </trdgrp>
  <trd lng="de">Besen</trd>
  <trdgrp lng="en">
    <trd>broom</trd>,
    <trd>sweeper</trd>
  </trdgrp>
  <trd lng="es">escoba</trd>
  <trd lng="fr">balai</trd>
  <trd lng="hu">sepr&udblac;</trd>
  <trdgrp lng="nl">
    <trd>bezem</trd>,
    <trd>veger</trd>,
    <trd>borstel<klr tip="ind"> (bezem)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_t;&c_l;&c_a;</trd>,
    <trd>&c_v;&c_je;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="tr">s&uuml;p&uuml;rge</trd>
</drv>

<drv mrk="bala.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="bala.el0i.FIG">
    <ref tip="dif" cel="bala.for0i.FIG">for<tld/>i</ref>
    <ekz>
      mi el<tld/>os la domon de Jerobeam, kiel oni el<tld/>as malpura&jcirc;on
      <fnt><bib>MT</bib>, <lok>&Re1; 14:10</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="br">skuba&ntilde; kuit</trd>
  <trd lng="fr">balayer<klr> (&eacute;liminer)</klr></trd>
  <trd lng="nl">leegvegen</trd>
  <trd lng="hu">els&ouml;p&ouml;r</trd>
</drv>

<drv mrk="bala.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="bala.for0i.FIG">
    <dif>
      Dispu&scirc;i, perforte <ref tip="super" cel="for.0igi">forigi</ref>:
      <ekz>
        la vento for<tld/>as la nubojn, la foliojn;
      </ekz>
      <ekz>
        la malnovaj pensmanieroj estas for<tld/>itaj.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">skuba&ntilde; kuit</trd>
  <trd lng="de">wegfegen</trd>
  <trd lng="fr">balayer<klr> (&eacute;liminer)</klr></trd>
  <trd lng="nl">wegvegen</trd>
  <trd lng="hu">els&ouml;p&ouml;r</trd>
</drv>
</art>
