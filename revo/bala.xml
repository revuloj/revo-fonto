<drv mrk="bala.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="bala.0i.malpurajxon">
    <dif>
      Forpu&scirc;i la polvon a&ubreve; malpura&jcirc;on per speciala
      ilo, konsistanta el fasko da vergetoj a&ubreve; haregoj:
      <ekz>
        se &ccirc;iu <tld/>os anta&ubreve; sia pordo, tiam en la tuta
        urbo estos ordo
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>escombrar</trd>,
      <trd>agranar</trd>
    </trdgrp>
    <trd lng="hu">seper</trd>
    <trdgrp lng="pl">
      <trd>zamiata&cacute;</trd>,
      <trd>zmiata&cacute;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#25481;&#21521; <pr>di&#xE0;oxi&#xE0;ng</pr></trd>,
    <trd>&#25171;&#x626B; <pr>d&#x1CE;s&#x1CE;o</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="bala.0i.FIG">
     <uzo tip="stl">FIG</uzo>
     <dif>
       <ref tip="dif" cel="bala.for0i.FIG">For<tld/>i</ref>:
       <ekz>
         la venkinta partio <tld/>is siajn
         kontra&ubreve;antojn.
       </ekz>
     </dif>
     <trdgrp lng="ca">
       <trd>bandejar</trd>,
       <trd>treure del mig</trd>,
       <trd>eliminar</trd>
     </trdgrp>
     <trd lng="pl">zamiata&cacute;</trd>
    <trdgrp lng="zh">
    <trd>&#25171;&#x626B; <pr>d&#x1CE;s&#x1CE;o</pr></trd>,
    <trd>&#25481;&#21521; <pr>di&#xE0;oxi&#xE0;ng</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_je;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_m;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_m;&c_je;&c_t;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_i;&c_t;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="bo">&#xF42;&#xF51;&#xF0B;&#xF51;&#xF62;&#xF0B;&#xF56;&#xFB1;&#xF7A;&#xF51;&#xF0B;</trd>
  <trd lng="br">skuba&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>m&eacute;st</trd>,
    <trd>sm&eacute;st</trd>,
    <trd>zametat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>kehren</trd>,
    <trd>fegen</trd>
  </trdgrp>
  <trd lng="en">sweep</trd>
  <trd lng="es">barrer</trd>
  <trdgrp lng="fa">
    <trd>&a_r;&a_w;&a_b;&f_y;&a_d;&a_n;</trd>,
    <trd>&a_j;&a_A;&a_r;&a_w; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">balayer</trd>
  <trd lng="he">&#x5DC;&#x5D8;&#x5B7;&#x5D0;&#x5D8;&#x5B5;&#x5D0;</trd>
  <trd lng="id">[<ind>sapu</ind>] menyapu</trd>
  <trdgrp lng="ja">
    <trd>&#25475;&#x304F;  <pr>&#x306F;&#x304F;</pr></trd>,
    <trd>&#x8D70;&#x67FB;&#12377;&#x308B;  <pr>&#x305D;&#12358;&#12373;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">vegen</trd>
  <trd lng="pt">varrer</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_m;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>zameta&tcaron;</trd>,
    <trd>zmies&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">sopa</trd>
  <trd lng="tr">s&uuml;p&uuml;rmek</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_je;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&#x43C;&c_ib;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_ib;&c_d;&#x43C;&c_ib;&c_t;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="bala.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="bala.0ajxo.balaitajxo">
    <dif>
      <tld/>ita&jcirc;o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiam ekkaptis ilin la uragano fortege
        kaj Ni faris ilin <tld/>a&jcirc;o
        <fnt>
          <aut>trad. I. Chiussi</aut>,
          <vrk>La Nobla Korano</vrk>,
          <lok>1970</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="br">
      <trd>skubadur</trd>,
      <trd>skubien</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>escombraries</trd>,
      <trd>fems</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>balayure</trd>,
      <trd>ordure</trd>
    </trdgrp>
    <trd lng="nl">veegsel</trd>
    <trd lng="en">sweepings</trd>
    <trd lng="pl">&sacute;mieci</trd>
    <trd lng="zh">&#22403;&#x573E; <pr>l&amacron;j&#x12B;</pr></trd>
</snc>
  <snc mrk="bala.0ajxo.senvalorajxo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senvalora&jcirc;o,
      <ref tip="vid" cel="bagate.0o">bagatelo</ref>,
      <ref tip="vid" cel="jxet.el0ajxo">el&jcirc;eta&jcirc;o</ref>,
      <ref tip="vid" cel="rub.0o">rubo</ref>.
    </dif>
    <trd lng="bg">&c_b;&c_o;&c_k;&c_l;&c_u;&c_k;</trd>
    <trdgrp lng="ca">
      <trd>brolleria</trd>,
      <trd>fotesa</trd>,
      <trd>bagatel&middot;la</trd>
    </trdgrp>
    <trd lng="fr">rebut</trd>
    <trdgrp lng="nl">
      <trd>afval</trd>,
      <trd>vuilnis</trd>
    </trdgrp>
    <trd lng="pl">&sacute;mieci</trd>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_m;&c_je;&c_c;&c_mol;&c_c;&c_je;</trd>
  <trd lng="ca">deixalles</trd>
  <trd lng="de">Kehricht</trd>
  <trdgrp lng="fa">
    <trd>&a_kh;&a_A;&f_k;&a_r;&a_w;&a_b;&a_h;</trd>,
    <trd>&a_A_madda;&a_sh;&a_gh;&a_A;&a_l;</trd>
  </trdgrp>
  <trd lng="he">&#x5DE;&#x5D4; &#x5E9;&#x5D8;&#x5D5;&#x5D0;&#x5D8;&#x5D0;</trd>
  <trd lng="hu">szem&eacute;t</trd>
  <trdgrp lng="ja">
    <trd>&#12385;&#x308A;</trd>,
    <trd>&#12372;&#x307F;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_r;</trd>,
    <trd>&c_m;&c_u;&c_s;&c_o;&c_r;</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&#x43C;&c_ib;&c_t;&c_t;&#x44F;</trd>
</drv>

<drv mrk="bala.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="bala.0ilo.HEJM">
    <dif>
      Ilo por <tld/>i:
      <ekz>
        sidi kiel muso sub <tld/>ilo
        <klr>(silenti, senmovi pro timo a&ubreve; singardo)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x7BF2; <pr>hu&#xEC;</pr></trd>,
    <trd>&#x57FD; <pr>s&#xE0;o</pr></trd>,
    <trd>&#x626B; <pr>s&#xE0;o</pr></trd>,
    <trd>&#x7B24;&#x5E1A; <pr>ti&#xE1;ozhou</pr></trd>,
    <trd>&#x5E1A; <pr>zh&#x1D2;u</pr></trd>,
    <trd>&#x626B;&#x628A; <pr>s&#xE0;ob&#x1CE;</pr></trd>,
    <trd>&#x626B;&#x5E1A; <pr>s&#xE0;ozhou</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ja;&c_t;&c_l;&c_a;</trd>,
    <trd>&c_v;&c_je;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_m;&c_je;&c_t;&c_l;&c_a;</trd>
  <trd lng="bo">&#xF55;&#xFB1;&#xF42;&#xF66;&#xF0B;&#xF58;&#xF0B;</trd>
  <trdgrp lng="br">
    <trd>balaenn</trd>,
    <trd>skubellenn</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>escombra</trd>,
    <trd>granera</trd>
  </trdgrp>
  <trd lng="cs">ko&scaron;t&ecaron;</trd>
  <trd lng="de">Besen</trd>
  <trdgrp lng="en">
    <trd>broom</trd>,
    <trd>sweeper</trd>
  </trdgrp>
  <trd lng="es">escoba</trd>
  <trd lng="fa">&a_j;&a_A;&a_r;&a_w;</trd>
  <trd lng="fr">balai</trd>
  <trd lng="he">&#x5DE;&#x5B0;&#x5D8;&#x5B7;&#x5D0;&#x5D8;&#x5B5;&#x5D0;</trd>
  <trd lng="hu">sepr&udblac;</trd>
  <trd lng="id">sapu</trd>
  <trdgrp lng="it">
    <trd>scopa</trd>,
    <trd>ramazza</trd>
  </trdgrp>
  <trd lng="ja">&#x307B;&#12358;&#x304D;</trd>
  <trdgrp lng="nl">
    <trd>bezem</trd>,
    <trd>veger</trd>,
    <trd>borstel<klr tip="ind"> (bezem)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>miot&lstroke;a</trd>,
    <trd>zmiotka</trd>
  </trdgrp>
  <trd lng="pt">vassoura</trd>
  <trd lng="ro">m&abreve;tur&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_t;&c_l;&c_a;</trd>,
    <trd>&c_v;&c_je;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">zmet&aacute;k</trd>
  <trd lng="tr">s&uuml;p&uuml;rge</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_ib;&c_t;&#x43B;&c_a;</trd>,
    <trd>&#x43F;&#x43E;&#x43C;&c_je;&#x43B;&#x43E;</trd>,
    <trd>&c_v;&c_ib;&#x43D;&c_i;&#x43A;</trd>
  </trdgrp>
</drv>

<drv mrk="bala.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="bala.el0i.FIG">
    <dif>
      <ref tip="dif" cel="bala.for0i.FIG">For<tld/>i</ref>:
      <ekz>
        mi el<tld/>os la domon de Jerobeam, kiel oni el<tld/>as
        malpura&jcirc;on
        <fnt><bib>MT</bib>, <lok>&Re1; 14:10</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x626B; <pr>s&#x1CE;o</pr></trd>
</snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_m;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_m;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_i;&c_z;&c_m;&c_i;&c_t;&c_a;&c_m;</trd>
  <trd lng="br">skuba&ntilde; kuit</trd>
  <trdgrp lng="ca">
    <trd>eliminar</trd>,
    <trd>treure del mig</trd>
  </trdgrp>
  <trd lng="cs">vym&eacute;st</trd>
  <trd lng="de">ausfegen</trd>
  <trd lng="fr">balayer<klr> (&eacute;liminer)</klr></trd>
  <trd lng="hu">els&ouml;p&ouml;r</trd>
  <trd lng="it"><ind>spazzare</ind> via</trd>
  <trd lng="nl">leegvegen</trd>
  <trdgrp lng="pl">
    <trd>wymiata&cacute;</trd>,
    <trd>zamiata&cacute;</trd>
  </trdgrp>
  <trd lng="sk">vymies&tcaron;</trd>
  <trd lng="uk">&c_v;&c_i;&#x43C;&c_ib;&c_t;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="bala.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="bala.for0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Dispu&scirc;i, perforte <ref tip="super" cel="for.0igi">forigi</ref>:
      <ekz>
        mi for<tld/>os de &gcirc;i &gcirc;ian polvon, kaj mi faros
        &gcirc;in nuda roko
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 26:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <klr>[...]</klr> for<tld/>os la postesignojn post vi
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 21:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu el la lastaj diktatoroj
de I&#8217; civilizita mondo estas for<tld/>ita de sia trono
        <fnt>

          <vrk><url>Estimataj gelegantoj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/11, p. 3</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vento for<tld/>as la nubojn, la foliojn;
      </ekz>
      <ekz>
        la malnovaj pensmanieroj estas for<tld/>itaj.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x6A2A;&#x626B; <pr>h&#xE9;ngs&#x1CE;o</pr></trd>,
    <trd>&#x626B;&#38500; <pr>s&#x1CE;och&#xFA;</pr></trd>,
    <trd>&#25152;&#21521;&#x62AB;&#38753; <pr>su&#x1D2;xi&#xE0;ngp&#x12B;m&#x1D0;</pr></trd>,
    <trd>&#21367; <pr>ju&#x1CE;n</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_m;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_m;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_o;&c_m;&c_i;&c_t;&c_a;&c_m;</trd>
  <trd lng="br">skuba&ntilde; kuit</trd>
  <trdgrp lng="ca">
    <trd>eliminar</trd>,
    <trd>treure del mig</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>odm&eacute;st</trd>,
    <trd>vymetat</trd>,
    <trd>vym&eacute;st</trd>,
    <trd>zam&eacute;st</trd>
  </trdgrp>
  <trd lng="de">wegfegen</trd>
  <trd lng="fr">balayer<klr> (&eacute;liminer)</klr></trd>
  <trd lng="hu">els&ouml;p&ouml;r</trd>
  <trd lng="id">[<ind>sapu</ind>] menyapu bersih</trd>
  <trdgrp lng="ja">
    <trd>&#25475;&#x304D;&#25448;&#12390;&#x308B;  <pr>&#x306F;&#x304D;&#12377;&#12390;&#x308B;</pr></trd>,
    <trd>&#x4E00;&#25475;&#12377;&#x308B;  <pr>&#12356;&#12387;&#x305D;&#12358;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">wegvegen</trd>
  <trdgrp lng="pl">
    <trd>wymiata&cacute;</trd>,
    <trd>zamiata&cacute;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odmies&tcaron;</trd>,
    <trd>zmies&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_z;&#x43C;&c_ib;&c_t;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="bala.bros0ilo">
  <kap>bros<tld/>ilo</kap>
  <snc mrk="bala.bros0ilo.HEJM">
    <dif>
      <ref tip="super" cel="bala.0ilo.HEJM"><tld lit="B"/>ilo</ref>
      kun brosa ekstremo por frotpurigi:
      <ekz>
        la patrino alkuris kun bros<tld/>ilo
        <fnt>
          <aut>V. V&aacute;&ncaron;a</aut>,
          <vrk>Kiel ni Mortigadis Leporon...</vrk>,
          <lok>1961</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_ch;&c_o;&c_t;&c_k;&c_a; &c_d;&c_l;&c_ja; &c_p;&c_a;&c_d;&c_l;&c_o;&c_g;&c_ib;</trd>
  <trd lng="ca">escombra-raspall</trd>
  <trd lng="cs">smet&aacute;k</trd>
  <trd lng="fr">balai-brosse</trd>
  <trd lng="sk">zmet&aacute;k</trd>
</drv>
