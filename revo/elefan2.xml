<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: elefan2.xml,v 1.13 2023/11/07 20:34:10 revo Exp $">
<kap>
  <rad>elefanted</rad>/oj
</kap>
<drv mrk="elefan2.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="rostr.0uloj">rostruloj</ref>
      (<trd lng="la">Elephantidae</trd>):
      <ekz>
        <ref tip="prt" cel="mamut.0o">mamuto</ref>
        estas <klr>[&#x2026;]</klr> jam formortinta <tld/>o, kiu vivis en la lasta
        glaciepoko grandnombre en Nord-Ameriko, E&ubreve;ropo, Azio kaj Afriko &gcirc;is la 40.
        longitudo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17764">Mamuto</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="elefan.0o">elefanto</ref>
  </snc>
  <trd lng="be">&c_s;&c_l;&c_a;&c_n;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">elef&agrave;ntids</trd>
  <trd lng="de">Elefanten</trd>
  <trd lng="fr">&eacute;l&eacute;phantid&eacute;s</trd>
  <trd lng="hu">elef&aacute;ntf&eacute;l&eacute;k</trd>
  <trd lng="pl">s&lstroke;oniowate</trd>
  <trd lng="pt">elefant&iacute;deos</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_o;&c_n;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_s;&c_l;&c_o;&c_n;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="uk">&#x441;&#x43B;&#x43E;&#x43D;&#x43E;&#x432;&#x456;</trd>
  <trd lng="zh">&#x8C61; <pr>xi&#xE0;ng</pr></trd>
</drv>
</art>
