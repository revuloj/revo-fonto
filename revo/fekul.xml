<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>fekul</rad>/o
</kap>
<drv mrk="fekul.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Man&gcirc;ebla ameleca pulvoro, ekstraktebla el grenoj, faboj, terpomoj kc. 
    </dif>
    <refgrp tip="sin">
      <ref cel="farun.0o">faruno</ref>,
      <ref cel="amel.0o">amelo</ref>,
      <ref cel="gri.0o">grio</ref>.
    </refgrp>
  </snc>
  <trd lng="ru">&c_k;&c_r;&c_a;&c_h;&c_m;&c_a;&c_l;</trd>
</drv>
</art>

