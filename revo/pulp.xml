<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>pulp</rad>/o
</kap>
<drv mrk="pulp.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">ANA</uzo>
    <dif>
      Mola a&ubreve; duonmola histo, el kiu konsistas kelkaj organoj:
      <ekz>
        lieno kaj la interno de la dentradikoj estas el <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cement.0o">cemento 3</ref>.
    </refgrp>
    <trd lng="de">Mark</trd>
    <trd lng="nl">pulpa</trd>
    <trd lng="ru">&c_p;&c_u;&c_l;&c_mol;&c_p;&c_a;</trd>
  </snc>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Mola kaj karneca substanco de fruktoj kaj legomoj.
    </dif>
    <trd lng="de">Fruchtfleisch</trd>
    <trdgrp lng="nl">
      <trd>pulp</trd>,
      <trd>vruchtvlees</trd>
    </trdgrp>
    <trd lng="ru">&c_m;&c_ja;&c_k;&c_o;&c_t;&c_mol;</trd>
  </snc>
</drv>
</art>
