<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kupon</rad>/o
</kap>
<drv mrk="kupon.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Bileto, detran&ccirc;ita de akcio a&ubreve; obligacio, rajtiganta la havanton
      ricevi difinitan procenton &ccirc;e iu bankiero.
    </dif>
    <trdgrp lng="de">
      <trd>Dividendenschein</trd>,
      <trd>Zinsschein</trd>,
      <trd>Coupon</trd>,
      <trd>Kupon</trd>
    </trdgrp>
    <trd lng="en">coupon</trd>
    <trd lng="nl">coupon</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_u;&c_p;&c_o;&c_n;</trd>,
      <trd>&c_o;&c_t;&c_r;&c_je;&c_z;&c_n;&c_o;&c_j; &c_t;&c_a;&c_l;&c_o;&c_n;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">TEA</uzo>
    <dif>
      Bileto, detran&ccirc;ita de registro, rajtiganta la havanton okupi lokon
      en teatro ktp.
    </dif>    
    <trdgrp lng="de">
       <trd>Abschnitt</trd>,
       <trd>Kontrollabschnitt</trd>,
       <trd>Marke</trd>
    </trdgrp>
    <trd lng="en">stub</trd>
    <trd lng="nl">kaartje</trd>
    <trd lng="ru">&c_k;&c_o;&c_n;&c_t;&c_r;&c_o;&c_l;&c_mol; <klr>(&c_t;&c_je;&c_a;&c_t;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_g;&c_o; &c_b;&c_i;&c_l;&c_je;&c_t;&c_a; &c_i; &c_t;.&c_p;.)</klr></trd>
  </snc>
</drv>
</art>
