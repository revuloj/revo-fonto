<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kupon.xml,v 1.25 2023/12/03 09:59:03 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>kupon</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kupon.0o">
  <kap><tld/>o</kap>
  <snc mrk="kupon.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="bilet.0o.KOMUNE">Bileto</ref>
      detondita a&ubreve; de&scirc;irita de registro a&ubreve; de pli granda folio:
      <ekz>
        legantoj de la Unua Libro estis petitaj subskribi kaj resendi al la a&ubreve;toro
        <tld/>ojn kun la jena promeso: &leftquot;mi, subskribi(n)ta, promesas ellerni la
        proponitan de d-ro Esperanto lingvon internacian&rightquot;
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Ligo emisiis t.n. Premio-<tld lit="K"/>ojn en valoro de unu stelo
        <klr>[&#x2026;]</klr> pro varbo de novaj membroj
        <fnt>
          <bib>Monato</bib>
          <aut>Lawrence Mee</aut>
          <vrk>La stelo</vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <subsnc mrk="kupon.0o.EKON">
      <uzo tip="fak">EKON</uzo>
      <dif>
        <ref tip="super" cel="bilet.0o.KOMUNE">Bileto</ref> detondita
        de <ref tip="malprt" cel="akci.0o">akcio</ref> a&ubreve; obligacio, rajtiganta la
        havanton ricevi difinitan dividendon a&ubreve; procenton.
      </dif>
      <trdgrp lng="de">
        <trd>Dividendenschein</trd>,
        <trd>Zinsschein</trd>,
        <trd>Coupon</trd>,
        <trd>Kupon</trd>
      </trdgrp>
      <trd lng="en">coupon</trd>
      <trdgrp lng="hu">
        <trd>osztal&eacute;kszelv&eacute;ny</trd>,
        <trd>kamatszelv&eacute;ny</trd>,
        <trd>szelv&eacute;ny</trd>,
        <trd>kupon</trd>
      </trdgrp>
      <trdgrp lng="it">
        <trd>buono<klr> (econ.)</klr></trd>,
        <trd>cedola<klr> (econ.)</klr></trd>
      </trdgrp>
      <trd lng="nl">coupon</trd>
      <trd lng="pl">kupon</trd>
      <trdgrp lng="ru">
        <trd>&c_k;&c_u;&c_p;&c_o;&c_n;</trd>,
        <trd>&c_o;&c_t;&c_r;&c_je;&c_z;&c_n;&c_o;&c_j; &c_t;&c_a;&c_l;&c_o;&c_n;</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="kupon.0o.TEA">
      <uzo tip="fak">TEA</uzo>
      <dif>
        <ref tip="super" cel="bilet.0o.KOMUNE">Bileto</ref> detondita
        de registro, rajtiganta la havanton okupi lokon en teatro ktp.
      </dif>
      <trdgrp lng="de">
        <trd>Abschnitt</trd>,
        <trd>Kontrollabschnitt</trd>,
        <trd>Marke</trd>
      </trdgrp>
      <trd lng="en">stub</trd>
      <trd lng="hu">jegy</trd>
      <trdgrp lng="it">
        <trd>tagliando</trd>,
        <trd>cedola</trd>,
        <trd>figlia<klr> (tagliando)</klr></trd>,
        <trd>talloncino<klr> (tagliando)</klr></trd>
      </trdgrp>
      <trd lng="nl">kaartje</trd>
      <trd lng="pl">odcinek</trd>
      <trd lng="ru">&c_k;&c_o;&c_n;&c_t;&c_r;&c_o;&c_l;&c_mol; <klr>(&c_t;&c_je;&c_a;&c_t;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_g;&c_o;
      &c_b;&c_i;&c_l;&c_je;&c_t;&c_a; &c_i;
      &c_t;.&c_p;.)</klr></trd>
    </subsnc>
    <subsnc mrk="kupon.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
       <ref tip="super" cel="bilet.0o.KOMUNE">Bileto</ref> dum- a&ubreve;
       post-milite rajtiganta la havanton ricevi a&ubreve;
       a&ccirc;eti difinitan nutra&jcirc;oporcion, vesta&jcirc;ojn, k.s.:
      <ekz>
        kun la salajro oni ricevis foliojn (de formato A4) kun fortran&ccirc;eblaj
        diversvaloraj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Dmitrij Cibulevskij</aut>
          <vrk><url ref="&Monato;/2011/010444.php">Kupono-mono</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
       <ekz>
          <ind>nutra&jcirc;<tld/>o</ind>j anka&ubreve; i&gcirc;as pli kaj pli
          senvaloraj, &ccirc;ar sur
          ili indikitaj nutra&jcirc;oj ne trovi&gcirc;as en vendejoj
           <fnt><aut>Stefan Maul</aut>,
           <vrk>El verva vivo jurnalista</vrk>, <lok>p. 39</lok></fnt>.
           <trd lng="fa">&f_k;&a_w;&f_p;&a_n; &a_gh;&a_dh;&a_A;</trd>
           <trd lng="nl">voedselcoupon</trd>
        </ekz>
      </dif>
      <trd lng="de">Lebensmittelmarke</trd>
      <trdgrp lng="it">
      <trd>bollino<klr> (buono per generi alimentari)</klr></trd>,
      <trd>tessera<klr> (per alimenti)</klr></trd>,
      <trd>cedola</trd>,
      <trd>buono<klr> pasto</klr></trd>
      </trdgrp>
      <trd lng="pl">kartki</trd>
      <trd lng="nl">coupon</trd>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="jxeton.0o">&jcirc;etono</ref>,
      <ref cel="mark.0o.objekto">marko<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_p;&c_o;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>kup&oacute;n</trd>,
    <trd>&uacute;st&rcaron;i&zcaron;ek</trd>
  </trdgrp>
  <trd lng="fa">&f_k;&a_w;&f_p;&a_n;</trd>
  <trd lng="fr">coupon</trd>
  <trdgrp lng="ja">
    <trd>&#x30AF;&#x30FC;&#x30DD;&#x30F3;</trd>,
    <trd>&#x5207;&#x7B26;&#x306E;&#x534A;&#x5238;  <pr>&#x304D;&#x3063;&#x3077;&#x306E;&#x306F;&#x3093;&#x3051;&#x3093;</pr></trd>,
    <trd>&#x5229;&#x672D;  <pr>&#x308A;&#x3055;&#x3064;</pr></trd>
  </trdgrp>
  <trd lng="sk">&uacute;stri&zcaron;ok</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_u;&c_p;&c_o;&c_n;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_r;&c_ib;&c_z;&c_n;&c_i;&c_j; &c_t;&c_a;&c_l;&c_o;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4F18;&#x60E0;&#x5238; <pr>y&#x14D;uhu&#xEC;qu&#xE0;n</pr></trd>,
    <trd>&#x512A;&#x60E0;&#x5238; <pr>y&#x14D;uhu&#xEC;qu&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kupon.internaciarespond0o">
  <kap>internacia respond<tld/>o</kap>
  <snc mrk="kupon.internaciarespond0o.POSX">
    <uzo tip="fak">POSX</uzo>
    <dif>
      En &ccirc;iuj landoj de la Universala Po&scirc;ta Unui&gcirc;o, inter&scirc;an&gcirc;ebla
      <ref tip="super" cel="bilet.0o.KOMUNE">bileto</ref> valora afrankon de simpla letero,
      kaj kiu tiel povas servi kiel internacia pagrimedo:
      <ekz>
        <klr>[por]</klr> uzi tiun &ccirc;i servon, <klr>[vi]</klr> estas petataj sendi &ccirc;iujn
        leterojn kun du internaciaj respond<tld/>oj al Universala Esperanto-Asocio
        <fnt>
          <bib>EeP</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        el ekonomia vidpunkto la sistemo de internaciaj respond<tld/>oj (IRK) estas
        unika&jcirc;o: per &gcirc;i eblas protekti sian investon kontra&ubreve; inflacio kaj kontra&ubreve;
        valut&scirc;an&gcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2007/009087.php">Internaciaj respondkuponoj: &ccirc;u fino alproksimi&gcirc;as?</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        niaj eksterlandaj abonantoj povas mendi <klr>[&#x2026;]</klr> rekte &ccirc;e ni sendinte 6
        internaciajn respond<tld/>ojn
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 7 (57)</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="de">Internationaler Antwortschein</trd>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_zh;&c_n;&c_a;&c_r;&c_o;&c_d;&c_n;&c_y; &c_a;&c_d;&c_k;&c_a;&c_z;&c_n;&c_y; <ind>&c_k;&c_u;&c_p;&c_o;&c_n;</ind></trd>
  <trdgrp lng="cs">
    <trd>mezin&aacute;rodn&iacute; odpov&ecaron;dka</trd>,
    <trd>mezin&aacute;rodn&iacute; odpov&ecaron;dn&iacute; kupon <klr>(po&scaron;tovn&iacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>international reply coupon</trd>,
    <trd>IRC</trd>
  </trdgrp>
  <trd lng="es">cup&oacute;n de respuesta internacional</trd>
  <trd lng="fa">&f_k;&a_w;&f_p;&a_n; &a_b;&f_y;&a_n;&#x200C;&a_A;&a_l;&a_m;&a_l;&a_l;&f_y; &f_p;&a_A;&a_s;&a_kh;</trd>
  <trd lng="fr">coupon-r&eacute;ponse international</trd>
  <trdgrp lng="hu">
    <trd>v&aacute;laszd&iacute;jszelv&eacute;ny</trd>,
    <trd>nemzetk&ouml;zi <ind>v&aacute;laszkupon</ind></trd>,
    <trd>kupon</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>buono di risposta internazionale</trd>,
    <trd>coupon<klr> (poste)</klr></trd>
    <trd>CRI<klr> (poste)</klr></trd>
    <trd>IRC<klr> (poste)</klr></trd>
  </trdgrp>
  <trd lng="nl">internationale antwoordcoupon</trd>
  <trdgrp lng="pl">
    <trd>mi&eogonek;dzynarodowy <ind>kupon</ind> na odpowied&zacute;</trd>,
    <trd>kupon pocztowy</trd>
  </trdgrp>
  <trd lng="sk">medzin&aacute;rodn&yacute; po&scaron;tov&yacute; kup&oacute;n</trd>
</drv>
</art>
