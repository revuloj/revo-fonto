<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kupon</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kupon.0o">
  <kap><tld/>o</kap>
  <snc mrk="kupon.0o.KOMUNE">
    <dif>
      &Scirc;ir<ref tip="super" cel="bilet.0o.KOMUNE">bileto</ref>, 
      kies stumpo restas, kaj kies &ccirc;efparton deprenas la uzanto.
    </dif>
    <subsnc mrk="kupon.0o.EKON">
      <uzo tip="fak">EKON</uzo>
      <dif>
        Bileto detran&ccirc;ita de akcio a&ubreve; obligacio, rajtiganta la havanton
        ricevi difinitan procenton &ccirc;e iu bankiero.
      </dif>
      <trdgrp lng="de">
        <trd>Dividendenschein</trd>,
        <trd>Zinsschein</trd>,
        <trd>Coupon</trd>,
        <trd>Kupon</trd>
      </trdgrp>
      <trd lng="en">coupon</trd>
      <trdgrp lng="hu">
        <trd>osztal&eacute;kszelv&eacute;ny</trd>,
        <trd>kamatszelv&eacute;ny</trd>,
        <trd>szelv&eacute;ny</trd>,
        <trd>kupon</trd>
      </trdgrp>
      <trd lng="nl">coupon</trd>
      <trdgrp lng="ru">
        <trd>&c_k;&c_u;&c_p;&c_o;&c_n;</trd>,
        <trd>&c_o;&c_t;&c_r;&c_je;&c_z;&c_n;&c_o;&c_j; &c_t;&c_a;&c_l;&c_o;&c_n;</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="kupon.0o.TEA">
      <uzo tip="fak">TEA</uzo>
      <dif>
        Bileto detran&ccirc;ita de registro, rajtiganta la havanton okupi lokon
        en teatro ktp.
      </dif>
      <trdgrp lng="de">
        <trd>Abschnitt</trd>,
        <trd>Kontrollabschnitt</trd>,
        <trd>Marke</trd>
      </trdgrp>
      <trd lng="en">stub</trd>
      <trd lng="hu">jegy</trd>
      <trd lng="nl">kaartje</trd>
      <trd lng="ru">&c_k;&c_o;&c_n;&c_t;&c_r;&c_o;&c_l;&c_mol; <klr>(&c_t;&c_je;&c_a;&c_t;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_g;&c_o; &c_b;&c_i;&c_l;&c_je;&c_t;&c_a; &c_i;
      &c_t;.&c_p;.)</klr></trd>
    </subsnc>
  </snc>
  <trd lng="fr">coupon</trd>
</drv>

<drv mrk="kupon.internaciarespond0o">
  <kap>internacia respond<tld/>o</kap>
  <snc mrk="kupon.internaciarespond0o.POSX">
    <uzo tip="fak">POSX</uzo>
    <dif>
      En &ccirc;iuj landoj de la Universala Po&scirc;ta Unui&gcirc;o inter&scirc;an&gcirc;ebla bileto,
      kiu tial povas servi kiel internacia pagrimedo.
    </dif>
  </snc>
  <trd lng="en">international reply coupon</trd>
  <trd lng="es">cup&oacute;n de respuesta internacional</trd>
  <trd lng="fr">coupon-r&eacute;ponse international</trd>
  <trdgrp lng="hu">
    <trd>v&aacute;laszd&iacute;jszelv&eacute;ny</trd>,
    <trd>nemzetk&ouml;zi <ind>v&aacute;laszkupon</ind></trd>,
    <trd>kupon</trd>
  </trdgrp>
  <trd lng="nl">internationale antwoordcoupon</trd>
</drv>
</art>
