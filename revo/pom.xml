<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pom</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pom.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pom.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Frukto de la <tld/>arbo.
    </dif>
    <ref tip="super" cel="frukt.plur0o">plurfrukto</ref>
  </snc>
  <snc mrk="pom.0o.FIG">
    <dif>
      Objekto kun formo de <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_b;&c_l;&c_y;&c_k;</trd>
  <trd lng="bg">&c_ja;&c_b;&c_malmol;&c_l;&c_k;&c_a;</trd>
  <trd lng="ca">poma</trd>
  <trd lng="cs">jablko</trd>
  <trd lng="de">Apfel</trd>
  <trd lng="en">apple</trd>
  <trd lng="es">manzana</trd>
  <trd lng="fa">&a_s;&f_y;&a_b;</trd>
  <trd lng="fr">pomme</trd>
  <trd lng="hu">alma</trd>
  <trd lng="nl">appel</trd>
  <trd lng="pl">jab&lstroke;ko</trd>
  <trd lng="pt">ma&ccedil;&atilde;</trd>
  <trd lng="ru">&c_ja;&c_b;&c_l;&c_o;&c_k;&c_o;</trd>
  <trd lng="tr">elma</trd>
  <trd lng="sv">&auml;pple</trd>
</drv>

<drv mrk="pom.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="pom.0ejo.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      Kampo kie pomarboj estas plantitaj.
    </dif>
  </snc>
  <trd lng="ca">pomereda</trd>
  <trd lng="be">&c_ja;&c_b;&c_l;&c_y;&c_n;&c_je;&c_v;&c_y; &c_s;&c_a;&c_d;</trd>
  <trd lng="br">avalenneg</trd>
  <trd lng="fa">&a_b;&a_A;&a_gh;
  <ind>&a_s;&f_y;&a_b;</ind></trd>
  <trd lng="fr">pommeraie</trd>
  <trd lng="nl">appelboomgaard</trd>
  <trd lng="pl">sad <klr>(jab&lstroke;kowy)</klr></trd> 
  <trd lng="ru">&c_ja;&c_b;&c_l;&c_o;&c_n;&c_je;&c_v;&c_y;&c_j; &c_s;&c_a;&c_d;</trd>
</drv>

<drv mrk="pom.0ujo">
  <kap><tld/>ujo, <var><kap><tld/>arbo</kap></var></kap>
  <snc mrk="pom.0ujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="rozac.0oj">rozacoj</ref>
      (<trd lng="la">Malus</trd>); unu specio (<trd lng="la">Malus
      domestica</trd>) estas kulturata en multaj varia&jcirc;oj.
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_b;&c_l;&c_y;&c_n;&c_ja;</trd>
  <trd lng="ca">pomera</trd>
  <trd lng="de">Apfelbaum</trd>
  <trd lng="es">manzano</trd>
  <trd lng="fa">&a_d;&a_r;&a_kh;&a_t;
  <ind>&a_s;&f_y;&a_b;</ind></trd>
  <trd lng="fr">pommier</trd>
  <trd lng="hu">almafa</trd>
  <trd lng="nl">appelboom</trd>
  <trd lng="pl">jab&lstroke;o&nacute;</trd>
  <trd lng="ru">&c_ja;&c_b;&c_l;&c_o;&c_n;&c_ja;</trd>
  <trd lng="sv">&auml;ppeltr&auml;d</trd>
</drv>

<drv mrk="pom.gorgxo0o">
  <kap>gor&gcirc;o<tld/>o,
  <var><kap>gor&gcirc;a <tld/>o</kap></var>,
  <var><kap>adam<tld/>o</kap></var></kap>
  <snc mrk="pom.gorgxo0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Elstaro de la <ref tip="vid" cel="tiroid.0o.ANA">tiroida</ref>
      kartilago videbla sur la kolo precipe de plenkreskaj viroj:
      <ekz>
        pro la tusado, lia viza&gcirc;o ru&gcirc;i&gcirc;is purpure, la gor&gcirc;a <tld/>o 
        levi&gcirc;is kaj mallevi&gcirc;is
        <fnt>
          <aut>Zhang Xingjian, trad. Miaohui</aut>,
          <vrk>La Sunperiodo Grenpluvo</vrk>,
          <lok>en <url
          ref="http://www.lulu.com/items/volume_64/4432000/4432358/1/print/lulu-traduko.pdf">
          Malgranda Stacidomo kaj aliaj Mikronoveloj</url>, 1990</lok>
        </fnt>;
      </ekz>
      <ekz>
        la adam<tld/>o en lia magra kolo dum
        la glutado sekvas liajn emociojn
        <fnt>
          <aut>S. Lang, trad. G. Berveling</aut>,
          <vrk>La Talpo&ccirc;asisto</vrk>,
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">pomme d'Adam</trd>
</drv>     
</art>
