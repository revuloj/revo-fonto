<drv mrk="pom.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="pom.0ejo.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      Kampo kie <tld/>arboj estas plantitaj.
    </dif>
  </snc>
  <trd lng="ca">pomereda</trd>
  <trd lng="be">&c_ja;&c_b;&c_l;&c_y;&c_n;&c_je;&c_v;&c_y; 
&c_s;&c_a;&c_d;</trd>
  <trd lng="br">avalenneg</trd>
  <trd lng="de">Apfelplantage</trd>
  <trd lng="fa">&a_b;&a_A;&a_gh;
  <ind>&a_s;&f_y;&a_b;</ind></trd>
  <trd lng="fr">pommeraie</trd>
  <trd lng="nl">appelboomgaard</trd>
  <trd lng="pl">sad <klr>(jab&lstroke;kowy)</klr></trd> 
  <trd lng="ru">&c_ja;&c_b;&c_l;&c_o;&c_n;&c_je;&c_v;&c_y;&c_j; 
&c_s;&c_a;&c_d;</trd>
</drv>

<drv mrk="pom.0ujo">
  <kap><ofc>*</ofc><tld/>ujo 
  <fnt><bib>F</bib>, <lok>Universala Vortaro, -uj'</lok></fnt>,
  <var><kap><tld/>arbo</kap></var></kap>
  <snc mrk="pom.0ujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="rozac.0oj">rozacoj</ref>
      (<trd lng="la">Malus</trd>) de
      <ref tip="lst" cel="arb.0o" lst="voko:rozoparencoj">arboj</ref>
      kaj arbustoj,
      kies unu specio (<trd lng="la">Malus
      domestica</trd>) estas kultivata en multaj varia&jcirc;oj
      pro la fruktoj.
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_b;&c_l;&c_y;&c_n;&c_ja;</trd>
  <trd lng="ca">pomera</trd>
  <trd lng="cs">jablo&ncaron;</trd>
  <trd lng="de">Apfelbaum</trd>
  <trd lng="es">manzano</trd>
  <trd lng="fa">&a_d;&a_r;&a_kh;&a_t;
  <ind>&a_s;&f_y;&a_b;</ind></trd>
  <trd lng="fr">pommier</trd>
  <trd lng="hu">almafa</trd>
  <trd lng="nl">appelboom</trd>
  <trd lng="pl">jab&lstroke;o&nacute;</trd>
  <trd lng="ru">&c_ja;&c_b;&c_l;&c_o;&c_n;&c_ja;</trd>
  <trd lng="sk">jablo&ncaron;</trd>
  <trd lng="sv">&auml;ppeltr&auml;d</trd>
<trd lng="tt">&c_a;&c_l;&c_m;&c_a;&c_g;&c_a;&c_ch;</trd>
</drv>

<drv mrk="pom.gorgxo0o">
  <kap>gor&gcirc;o<tld/>o,
  <var><kap>gor&gcirc;a <tld/>o</kap></var>,
  <var><kap>adam<tld/>o</kap></var></kap>
  <snc mrk="pom.gorgxo0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Elstaro de la tiroida
      kartilago videbla sur la kolo precipe de plenkreskaj viroj:
      <ekz>
        pro la tusado, lia viza&gcirc;o ru&gcirc;i&gcirc;is purpure, la 
gor&gcirc;a <tld/>o 
        levi&gcirc;is kaj mallevi&gcirc;is
        <fnt>
          <aut>Zhang Xingjian, trad. Miaohui</aut>,
          <vrk>La Sunperiodo Grenpluvo</vrk>,
          <lok>en <url ref="http://www.lulu.com/items/volume_64/4432000/4432358/1/print/lulu-traduko.pdf">
          Malgranda Stacidomo kaj aliaj Mikronoveloj</url>, 1990</lok>
        </fnt>;
      </ekz>
      <ekz>
        la adam<tld/>o en lia magra kolo dum
        la glutado sekvas liajn emociojn
        <fnt>
          <aut>S. Lang, trad. G. Berveling</aut>,
          <vrk>La Talpo&ccirc;asisto</vrk>,
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Adamsapfel</trd>
  <trd lng="fr">pomme d'Adam</trd>
  <trd lng="id">jakun</trd>
  <trdgrp lng="pl">
    <trd>grdyka</trd>,
    <trd>jab&lstroke;ko Adama <klr>(pot.)</klr></trd>
  </trdgrp>
</drv>     

<drv mrk="pom.stel0o">
  <kap>stel<tld/>o</kap>
  <snc mrk="pom.stel0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;ebla <ref tip="super" cel="frukt.0o.KUI">frukto</ref> de 
      stel<tld/>arbo, kun globaj, dol&ccirc;aj fruktoj, kiuj post 
tratran&ccirc;o
      elmontras stelforman hela&jcirc;on &ccirc;irka&ubreve; la semoj:
      <ekz>
        parencaj specioj, anka&ubreve; nomitaj stel<tld/>oj
        estas kultivitaj en Afriko, inkluzive de
        C. albidum kaj C. africanum
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Stelpomo">
          stelpomo</url>, 2013-02-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Sternapfel</trd>
  <trd lng="fr">pomme de lait</trd>
</drv>

<drv mrk="pom.stel0arbo">
  <kap>stel<tld/>arbo, <var><kap>stel<tld/>ujo</kap></var></kap>
  <snc mrk="pom.stel0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Alta <ref tip="lst" cel="arb.0o" lst="voko:erikoparencoj">arbo</ref> 
      el Centrameriko, Karibio, Antiloj
      donanta fruktojn kaj plurajn kuracajn substancojn;
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:botanikaj_specioj">specio</ref> el familio <ref tip="malprt" cel="sapotac.0oj.BOT">sapotacoj</ref>
            (<trd lng="la">Chrysophyllum cainito</trd>).
    </dif>
  </snc>
  <trd lng="de">Sternapfelbaum</trd>
  <trd lng="fr">ca&iuml;nitier</trd>
  <trd lng="zh">&#x91D1;&#x661F;&#x679C; [<ind>j&#x12B;n x&#x12B;ng gu&#x1D2;</ind>]</trd>
</drv>

<drv mrk="pom.suker0o">
  <kap>suker<tld/>o</kap>
  <snc mrk="pom.suker0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;ebla <ref tip="super" cel="frukt.0o.KUI">frukto</ref>
      de 
      <ref tip="malprt" cel="pom.suker0arbo">suker<tld/>arbo</ref>, kun
      plejofte
      verda skvameca &scirc;elo kaj je matureco blanka kaj mola
      karno:
      <ekz>
        ne forgeseblaj fruktoj estas drakofrukto
        (<ctl>Thanh long</ctl>) kaj suker<tld/>o
        (<ctl>m&atilde;ng c&#x1EA7;u ta</ctl>) por oferi al prapatroj
        <fnt>
          <aut>Rejko</aut>,
          <vrk><url ref="http://blog.livedoor.jp/deziras_pacon/archives/51422109.html">
          Preparado por Tet'o</url></vrk>,
          <lok>2009-01-22</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Zimtapfel</trd>
  <trd lng="en">sugar-apple</trd>
  <trdgrp lng="fr">
    <trd>atte</trd>,
    <trd>pomme cannelle</trd>
  </trdgrp>
</drv>

<drv mrk="pom.suker0arbo">
  <kap>suker<tld/>arbo, <var><kap>suker<tld/>ujo</kap></var></kap>
  <snc mrk="pom.suker0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Sudamerikdevena 
      <ref tip="lst" cel="arb.0eto" lst="voko:magnolioparencoj">arbeto</ref>
       kultivata en tropikaj kaj subtropikaj landoj pro la fruktoj;
       <ref tip="lst" cel="speci.0o.BIO" lst="voko:botanikaj_specioj">specio</ref>
      de <ref tip="super" cel="anon.0arbo">anonarbo</ref>
      (<trd lng="la">Annona squamosa</trd>):
      <ekz>
        la kunmetita frukto de la suker<tld/>arbo havas rondan formon
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Sukerpomo">
          sukerpomo</url>, 2013-03-12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Zimtapfel</trd>,
    <trd>S&uuml;&szlig;sack</trd>,
    <trd>Rahmapfel</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>attier</trd>,
    <trd>pommier cannelle</trd>
  </trdgrp>
  <trd lng="pl">flaszowiec &lstroke;uskowaty</trd>
</drv>
</art>
