<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>pom</rad>/o
</kap>
<drv mrk="pom.0o">
<kap><ofc>*</ofc><tld/>o</kap>
<snc num="1">
<dif>
Frukto de <tld/>arbo. 
</dif>
</snc>
<snc num="2">
<dif>
Objekto kun formo de <tld/>o. 
</dif>
</snc>
<trd lng="en">apple</trd>
<trd lng="fr">pomme</trd>
</drv>
<drv mrk="pom.0ujo0arbo">
<kap><tld/>ujo, <tld/>arbo</kap>
<snc>
<dif>
Arbo el fam. rozacoj (<trd lng="la">pirus malus</trd>). 
</dif>
</snc>
<trd lng="fr">pommier</trd>
</drv>
<drv mrk="pom.0vino">
<kap><tld/>vino</kap>
<uzo tip="fak">KUI</uzo>
<snc>
<dif>
Fermentanta trinka&jcirc;o elpremita el <tld/>oj.  
</dif>
</snc>
<trd lng="fr">cidre</trd>
</drv>
</art>
