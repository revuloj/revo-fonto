<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>pom</rad>/o
</kap>
<drv mrk="pom.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      &Scirc;ajnfrukto de <tld/>arbo.
    </dif>
  </snc>
  <snc>
    <dif>
      Objekto kun formo de <tld/>o.
    </dif>
  </snc>
  <trd lng="cs">jablko</trd>
  <trd lng="de">Apfel</trd>
  <trd lng="en">apple</trd>
  <trd lng="nl">appel</trd>
  <trd lng="pl">jab&lstroke;ko</trd>
  <trd lng="fr">pomme</trd>
  <trd lng="tr">elma</trd>
</drv>
<drv mrk="pom.0ujo0arbo">
  <kap><tld/>ujo, <tld/>arbo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="roz.0acoj">rozacoj</ref>
      (<trd lng="la">Malus</trd>), unu specio (<trd lng="la">Malus
      domestica</trd>) estas kulturata en multaj varia&jcirc;oj.
    </dif>
  </snc>
  <trd lng="de">Apfelbaum</trd>
  <trd lng="fr">pommier</trd>
  <trd lng="nl">appelboom</trd>
</drv>
<drv mrk="pom.0vino">
  <kap><tld/>vino</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Fermentanta trinka&jcirc;o elpremita el <tld/>oj.
    </dif>
  </snc>
  <trd lng="de">Apfelwein</trd>
  <trd lng="fr">cidre</trd>
  <trd lng="nl">cider</trd>
</drv>
</art>
