<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pom</rad>/o
</kap>
<drv mrk="pom.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Frukto de la <tld/>arbo.
    </dif>
    <ref tip="super" cel="frukt.plur0o">plurfrukto</ref>
  </snc>
  <snc>
    <dif>
      Objekto kun formo de <tld/>o.
    </dif>
  </snc>
  <trd lng="cs">jablko</trd>
  <trd lng="de">Apfel</trd>
  <trd lng="en">apple</trd>
  <trd lng="fr">pomme</trd>
  <trd lng="hu">alma</trd>
  <trd lng="nl">appel</trd>
  <trd lng="pl">jab&lstroke;ko</trd>
  <trd lng="ru">&c_ja;&c_b;&c_l;&c_o;&c_k;&c_o;</trd>
  <trd lng="tr">elma</trd>
  <trd lng="sv">&auml;pple</trd>
</drv>

<drv mrk="pom.0ujo0arbo">
  <kap><tld/>ujo, <tld/>arbo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="rozac.0oj">rozacoj</ref>
      (<trd lng="la">Malus</trd>), unu specio (<trd lng="la">Malus
      domestica</trd>) estas kulturata en multaj varia&jcirc;oj.
    </dif>
  </snc>
  <trd lng="de">Apfelbaum</trd>
  <trd lng="fr">pommier</trd>
  <trd lng="hu">almafa</trd>
  <trd lng="nl">appelboom</trd>
  <trd lng="ru">&c_ja;&c_b;&c_l;&c_o;&c_n;&c_ja;</trd>
  <trd lng="sv">&auml;ppeltr&auml;d</trd>
</drv>

<drv mrk="pom.ter0o">
  <kap><ofc>*</ofc><tld/>pomo</kap>
  <uzo tip="fak">BOT</uzo>
  <uzo tip="fak">KUI</uzo>
  <snc mrk="pom.ter0o.mangxajxo">
    <dif>
      Man&gcirc;ebla tubero esti&gcirc;anta &ccirc;e la radikoj de
      <tld/>pomo 2.
    </dif>
  </snc>
  <snc mrk="pom.ter0o.planto">
    <dif>
      Specio de <ref tip="super" cel="solan.0o">solano</ref>
      (<trd lng="la">Solanum tuberosum</trd>), devena de Ameriko
      kun man&gcirc;eblaj radiktuberoj.
    </dif>
  </snc>
  <trd lng="de">Kartoffel</trd>
  <trd lng="fr">pomme de terre</trd>
  <trd lng="nl">aardappel</trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_t;&c_o;&c_f;&c_je;&c_l;&c_mol;</trd>
  <trd lng="tr">patates</trd>
</drv>

<drv mrk="pom.0vino">
  <kap><tld/>vino</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Fermentanta trinka&jcirc;o elpremita el <tld/>oj.
    </dif>
  </snc>
  <trd lng="de">Apfelwein</trd>
  <trd lng="fr">cidre</trd>
  <trd lng="hu">almabor</trd>
  <trd lng="nl">cider</trd>
  <trd lng="ru">&c_s;&c_i;&c_d;&c_r;</trd>
</drv>
</art>
