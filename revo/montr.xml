<drv mrk="montr.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="montr.el0i.eksteren">
    <dif>
      Eksteren <tld/>i:
      <ekz>
        malpacienculo el<tld/>as malsa&gcirc;econ
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 14:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;in]</klr> maltrankviligis la obstina silentado de la infano eble pli, ol
        la el<tld/>ita doloro
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi povus el<tld/>i en mi tiajn aferojn, ke estus pli bone, se mia patrino min ne
        naskus
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        li konsciis sian senkulpecon, <klr>[sed]</klr> &ccirc;i tie ne povis ja el<tld/>i sian
        pravecon
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u tio estis la forto de la pre&gcirc;o <klr>[kiu]</klr> &ccirc;i tie sin el<tld/>is
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <snc mrk="montr.el0i.publike">
    <dif>
      Plene <tld/>i, <ref tip="sin" cel="ekspoz1.0i">ekspozicii</ref>:
      <ekz>
        ili havas kelkajn el<tld/>atajn skribma&scirc;inojn
        <fnt>
          <bib>ChC</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> lokis grandan el<tld/>on de kuiriloj en la fenestro de magazeno
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili rapidas de el<tld/>ejo al el<tld/>ejo, de strato al strato
        <fnt>
          <bib>Tor</bib>
        </fnt>;
      </ekz>
      <ekz>
        okazigo de la unua el<tld/>o de la Universala Ekspozicio de Esperanto en
        Montevideo
        <fnt>
          <bib>EeP</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_k;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_e;&c_m;&c_a;&c_n;&c_s;&c_t;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
