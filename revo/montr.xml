  </snc>
  <trd lng="be">&c_g;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_mol;&c_n;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_ja; &c_s;&c_t;&c_r;&c_e;&c_l;&c_k;&c_a;</trd>
