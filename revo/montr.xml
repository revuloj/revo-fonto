<drv mrk="montr.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="montr.0igxi.videbligxi">
    <dif>
      Videbli&gcirc;i:
      <ekz>
        aperas herbo, <tld/>i&gcirc;as verda&jcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 27:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        la suno montri&gcirc;is, kaj <klr>[&#8230;]</klr> sub &gcirc;iaj radioj <klr>[&#8230;]</klr> montri&gcirc;adis
        la bela figuro
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;irka&ubreve; la flamo de kandelo <tld/>i&gcirc;as &ccirc;ielarkaj koloroj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[jen]</klr> <tld/>i&gcirc;is herba loko &ccirc;irka&ubreve;ita de la arbaro
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        grizeco <klr>[&#8230;]</klr> <tld/>i&gcirc;is en liaj haroj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        sur la strato <tld/>i&gcirc;as rapide veturanta kale&scirc;o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la <klr>[&#8230;]</klr> deka monato <tld/>i&gcirc;is la suproj de la montoj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 8:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam la tavolo da roso forlevi&gcirc;is, tiam <tld/>i&gcirc;is, ke sur la supra&jcirc;o de la
        dezerto ku&scirc;as io delikata
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 16:14</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="nl">zich <ind>vertonen</ind></trd>
  </snc>
  <snc mrk="montr.0igxi.evidentigxi">
    <dif>
      <ref tip="sin" cel="eviden.0igxi">Evidenti&gcirc;i</ref>:
      <ekz>
        tiu &ccirc;i anta&ubreve;diro <tld/>i&gcirc;is ne tiel dan&gcirc;era
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;is, ke &gcirc;emeloj estas en &scirc;ia ventro
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 25:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi <tld/>i&gcirc;is favora al mi
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 33:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        kio en teorio &scirc;ajnis afero tiel facila, en la praktiko <tld/>i&gcirc;as tre malfacila
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        ekzistas nenia eblo anta&ubreve;vidi <klr>[&#8230;]</klr> &ccirc;u la finita laboro en la praktiko
        <tld/>i&gcirc;os tute ta&ubreve;ga
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        vera opinio <tld/>i&gcirc;as en ebrio
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">sich <ind>erweisen</ind></trd>
    <trd lng="nl">blijken</trd>
  </snc>
  <snc mrk="montr.0igxi.sin">
    <dif>
      Sin <tld/>i:
      <ekz>
        vi <tld/>i&gcirc;is malforta en tago de mizero
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 24:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        de l' frua infaneco li <tld/>i&gcirc;is jam poeta
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        iafoje &ccirc;ambristino en<tld/>i&gcirc;as <klr>[&#8230;]</klr>
        ridetas kaj skuas la kapon
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        neniam <tld/>i&gcirc;u en mia bando
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>!
      </ekz>
    </dif>
    <trd lng="fr">s'<ind>av&eacute;rer</ind></trd>
    <trd lng="nl">blijken</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_k;&c_a;&c_z;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_mol;&c_ja;&c_w;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>apar&eacute;ixer</trd>,
    <trd>mostrar-se</trd>
  </trdgrp>
  <trd lng="de">sich <ind>zeigen</ind></trd>
  <trdgrp lng="es">
    <trd>aparecer</trd>,
    <trd>mostrar-se</trd>
  </trdgrp>
  <trd lng="fr">se <ind>montrer</ind></trd>
  <trdgrp lng="pl">
    <trd>pojawia&cacute; si&eogonek;</trd>,
    <trd>ukazywa&cacute; si&eogonek;</trd>,
    <trd>okazywa&cacute; si&eogonek;</trd>,
    <trd>wydawa&cacute; si&eogonek;</trd>,
    <trd>przebija&cacute; si&eogonek;</trd>,
    <trd>zjawia&cacute; si&eogonek;</trd>,
    <trd>pokazywa&cacute; si&eogonek;</trd>,
    <trd>stawia&cacute; si&eogonek;</trd>
  </trdgrp>  
  <trdgrp lng="ro">
    <trd>ap&abreve;rea</trd>,
    <trd>se ivi</trd>,
    <trd>se ar&abreve;ta</trd>,
    <trd>p&abreve;rea</trd>,
    <trd>str&abreve;punge</trd>
  </trdgrp>  
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_k;&c_a;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_ja;&c_v;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_ja;&c_v;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_k;&c_a;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_i;&c_ja;&c_v;&c_l;&c_ja;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
