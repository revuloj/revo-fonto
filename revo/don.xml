<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>don</rad>/i
    </kap>
    <drv mrk="don.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Meti en posedon a&ubreve; je dispono de iu, &ccirc;u provizore,
          &ccirc;u definitive:
          <ekz>
            <tld/>i ludilon al infano;
          </ekz><ekz>
            <tld/>i al iu salajron, rekompencon;
          </ekz><ekz>
            <tld/>i al iu por man&gcirc;i kaj trinki;
          </ekz><ekz>
            <tld/>i doton, nomon, garantion, sian vo&ccirc;on, ekzemplon, edukon,
            helpon, favoron, honoron, atenton, konsenton, liberecon, pruvon;
          </ekz><ekz>
            <tld/>i balon, feston;
          </ekz><ekz>
            la semo <tld/>os fruktojn;
          </ekz><ekz>
            nur senvaloraj objektoj <tld/>is
            <klr>(liveris)</klr> por tiu dekoracio
            la materialon;
          </ekz><ekz>
            <tld/>i <klr>(etendi)</klr> al iu la manon<fnt>B</fnt>;
          </ekz><ekz>kolero pravecon ne <tld/>as<fnt><bib>PrV</bib></fnt>;</ekz><ekz>
            la &ccirc;ielo <tld/>u, ke...;
          </ekz><ekz>&ccirc;ia <ind><tld/>o</ind> estas bono
            <fnt><bib>PrV</bib></fnt>.
            <trd lng="fr">don</trd></ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="donac.0i">donaci</ref>,
          <ref cel="ofer.0i">oferi</ref>,
          <ref cel="konsen.0i">konsenti</ref>.
        </refgrp>
      </snc>
      <snc>
        <uzo tip="stl">FIG</uzo>
        <dif>
          Estigi, rezultigi, fari agon:
          <ekz>
            <tld/>i rezultaton<fnt>Z</fnt>;
          </ekz><ekz>
            la semoj <tld/>is <klr>(produktis)</klr>
            trunketojn<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>i <klr>(fari)</klr> signojn per la okuloj
            <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
              <lok>Sentencoj 6, 13</lok>)</fnt>;
          </ekz><ekz>
            <tld/>i baton, vangofrapon;
          </ekz><ekz>
            <tld/>i al iu sur la nazon<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>i lecionon, ju&gcirc;on, la&ubreve;don, raporton,
            sankcion, punon, instrukcion, ordonon, komision, la tonon;
          </ekz><ekz>
            <tld/>i profiton;
          </ekz><ekz>
            <tld/>i bonan impreson pri;
          </ekz><ekz>
            <tld/>i esperon, kontentigon, anta&ubreve;enpu&scirc;on,
            karakteron de, vidon al;
          </ekz><ekz>
            tiuj formoj <tld/>us grandan
            konfuzon<fnt>Z</fnt>;
          </ekz><ekz>
            tiu odoro <tld/>as kapturni&gcirc;on;
          </ekz><ekz>
            tia elparolado <tld/>as nenian
            malkompreni&gcirc;on<fnt>Z</fnt>;
          </ekz><ekz>
            se simila lingvo e&ccirc; povus esti finita, &gcirc;i nenion
            <tld/>us<fnt>Z</fnt>;
          </ekz><ekz>
            en la <tld/>ita <klr>(difinita, tiama, konsiderata)</klr>
            okazo<fnt>Z</fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="cs">d&aacute;t</trd>
      <trd lng="de">geben</trd>
      <trd lng="en">give</trd>
      <trd lng="fr">donner</trd>
      <trd lng="ru">&c_d;&c_a;&c_t;&c_mol;</trd>
      <trd lng="tr">vermek</trd>
    </drv>
    <drv mrk="don.0itajxo">
      <kap><tld/>ita&jcirc;o</kap>
      <snc mrk="don.0itajxo.SCI">
        <uzo tip="fak">SCI</uzo>
        <dif>
          Datumo, grando, variablo konata je la etapo de problemformulado
          kaj uzata por trovi ser&ccirc;ata&jcirc;on:
          <ekz>
            la <tld/>ita&jcirc;oj de la problemo.
          </ekz>
        </dif>
        <ref tip="sin" cel="datum.0o.SCI">datumo</ref>
        <ref tip="super" cel="datum.0o.KOMP">datumo</ref>
        <rim>
          PIV donas por tiu senco anka&ubreve; la formon <tld/>ato;
          eblus diri simple <tld/>a&jcirc;o.
        </rim>
        <trd lng="en">datum</trd>
        <trd lng="ru">&c_d;&c_a;&c_n;&c_n;&c_o;&c_je;</trd>
      </snc>
      <snc mrk="don.0itajxo.aksiomo">
        <dif>
          Io akceptita a&ubreve; konata kaj formanta bazon de esploro, rezonado
          a&ubreve; konduto:
          <ekz>
            akceptu tion kiel <tld/>ita&jcirc;on.
          </ekz>
        </dif>
        <ref tip="vid" cel="aksiom.0o">aksiomo</ref>
        <trd lng="en">smth taken for <ind>granted</ind></trd>
        <trd lng="ru">&c_d;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
      </snc>
      <trd lng="fr">donn&eacute;e</trd>
    </drv>
    <drv mrk="don.al0i">
      <kap>al<tld/>i</kap>
      <snc>
        <dif>
          <tld lit="D"/>i, meti, fari, diri plie, krom tio, kion oni 
          jam <tld/>is, metis, faris, diris:
          <ekz>
            nenion al<tld/>u al via respondo;
          </ekz><ekz>
            al<tld/>i al vorto la fina&jcirc;on <em>n</em>;
          </ekz><ekz>
            tagoj al<tld/>i&gcirc;as al tagoj kaj la situacio ne
            &scirc;an&gcirc;i&gcirc;as;
          </ekz><ekz>
            &jcirc;us aperis la kvara <ind>al<tld/>o</ind> al la Fundamento;
            <trd lng="fr">ajout</trd>
          </ekz><ekz>
            esti <ind>al<tld/>ita</ind> al iu
            <klr>(esti preta servi &ccirc;iuokaze al iu)</klr>. 
            <trdgrp lng="fr">
              <trd>attach&eacute;<klr tip="ind"> (d&eacute;vou&eacute;)</klr></trd>,
              <trd>d&eacute;vou&eacute;</trd></trdgrp> 
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="vic">alvicigi</ref>,
          <ref cel="al">aligi</ref>.
        </refgrp>
      </snc>
      <trd lng="de">hinzuf&uuml;gen</trd>
      <trd lng="fr">ajouter</trd>
      <trd lng="ru">&c_d;&c_o;&c_b;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>
    </drv>
    <drv mrk="don.al0iteco">
      <kap>al<tld/>iteco</kap>
      <snc>
        <dif>
          Emo servi &ccirc;iuokaze al iu.
        </dif>
        <refgrp tip="vid">
          <ref cel="don.sin0ema">sindonema</ref>.
        </refgrp>
      </snc>
      <trd lng="de">Hingabe</trd>
      <trd lng="fr">d&eacute;vouement</trd>
      <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
    </drv>
    <drv mrk="don.el0i">
      <kap>el<tld/>i</kap>
      <snc>
        <dif>
          Publikigi kaj vendigi ian ajn verkon:
          <ekz>
            el<tld/>i vortaron;
          </ekz><ekz>
            <ind>el<tld/>isto</ind>;
            <trd lng="fr">&eacute;diteur</trd>
          </ekz><ekz>
            la dua <ind>el<tld/>o</ind> estas jam el&ccirc;erpita
            <trd lng="fr">&eacute;dition</trd>
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="aper.0igi">aperigi</ref>,
          <ref cel="publik.0igi">publikigi</ref>.
        </refgrp>
      </snc>
      <trd lng="de">herausgeben</trd>
      <trd lng="fr">&eacute;diter</trd>
      <trd lng="ru">&c_i;&c_z;&c_d;&c_a;&c_t;&c_mol;</trd>
    </drv>
    <drv mrk="don.el0kvanto">
      <kap>el<tld/>kvanto</kap>
      <snc>
        <uzo tip="fak">TIP</uzo>
        <dif>
          Nombro de la ekzempleroj produktitaj por certa el<tld/>o:
          <ekz>
            la bro&scirc;uro aperis en tre malgranda el<tld/>kvanto
            <fnt>
              <aut>Ulrich Lins</aut>:
              <vrk>La dan&gcirc;era lingvo</vrk>,
              <lok>Anta&ubreve;parolo de la 2a eldono</lok>.</fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="fr">tirage<klr tip="ind"> (d'une publication)</klr></trd>
      <trd lng="ru">&c_t;&c_i;&c_r;&c_a;&c_zh;</trd>
    </drv>
    <drv mrk="don.dis0i">
      <kap>dis<tld/>i</kap>
      <snc>
        <dif>
          <tld lit="D"/>i al multaj, distribui:
          <ekz>nobelo promesojn disdonas kampulo promesojn plenumas<fnt><bib>PrV</bib></fnt>.</ekz> 
        </dif>
      </snc>
      <trd lng="de">verteilen</trd>
      <trdgrp lng="fr">
        <trd>distribuer</trd>,
        <trd>r&eacute;partir</trd></trdgrp> 
      <trd lng="ru">&c_r;&c_a;&c_z;&c_d;&c_a;&c_t;&c_mol;</trd>
    </drv>
    <drv mrk="don.for0i">
      <kap>for<tld/>i</kap>
      <snc>
        <dif>
          Tute kaj definitive <tld/>i:
          <ekz>
            li for<tld/>is al la grupo sian tutan profiton;
          </ekz><ekz>
            sin for<tld/>i al la propagando, al sporto, al siaj pasioj;
          </ekz><ekz>
            kun plena amo &scirc;i for<tld/>i&gcirc;is al la objekto de sia
            laborado<fnt>Z</fnt>.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="las.for0i">forlasi</ref>,
          <ref cel="ofer.0i">oferi</ref>,
          <ref cel="dedicx.0i">dedi&ccirc;i</ref>.
        </refgrp>
      </snc>
      <trdgrp lng="fr">
        <trd>faire <ind>don</ind> (de)</trd>,
        <trd>sacrifier<klr tip="ind"> (faire don de qc)</klr></trd></trdgrp> 
      <trd lng="ru">&c_o;&c_t;&c_d;&c_a;&c_t;&c_mol;</trd>
    </drv>
    <drv mrk="don.re0i">
      <kap>re<tld/>i</kap>
      <snc>
        <dif>
          <tld lit="D"/>i returne al la <tld/>into ion, kion oni ricevis:
          <ekz>
            li ankora&ubreve; ne povis re<tld/>i la pruntitan monon;
          </ekz><ekz>
            re<tld/>i salutojn<fnt>Z</fnt>, bondezirojn<fnt>Z</fnt>
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="recipr.0i">reciproki</ref>,
          <ref cel="pag.re0i">repagi</ref>.
        </refgrp>
        <trd lng="ru">&c_o;&c_t;&c_d;&c_a;&c_t;&c_mol;</trd>
      </snc>
      <snc>
        <dif>
          Rekompenci:
          <ekz>
            re<tld/>i al &ccirc;iu la&ubreve; lia konduto
            <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
              <lok>Jeremia 17, 10</lok>)</fnt>.
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="pag.re0i">repagi</ref>.
        </refgrp>
        <trd lng="ru">&c_v;&c_o;&c_z;&c_d;&c_a;&c_t;&c_mol;</trd>
      </snc>
      <snc>
        <dif>
          Puni per samo pro tio, kion oni suferis:
          <ekz>
            mi re<tld/>os al vi &ccirc;iujn ofendojn viajn;
          </ekz><ekz>
            tio estas justa re<tld/>o<fnt>Z</fnt>
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="pag.re0i">repagi</ref>.
        </refgrp>
        <trd lng="ru">&c_v;&c_o;&c_z;&c_d;&c_a;&c_t;&c_mol;</trd>
      </snc>
      <snc>
        <dif>
          Kopii, reprodukti:
          <ekz>
            la portreto &gcirc;uste re<tld/>as lian
            fizionomion;
          </ekz><ekz>
            lia stilo re<tld/>as lian karakteron.
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="spegul.0i">speguli</ref>.
        </refgrp>
        <trdgrp lng="ru">
          <trd>&c_v;&c_o;&c_s;&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_i;</trd>
          <trd>&c_o;&c_t;&c_o;&c_b;&c_r;&c_a;&c_z;&c_i;&c_t;&c_mol;</trd>
          <trd>&c_p;&c_je;&c_r;&c_je;&c_d;&c_a;&c_t;&c_mol;</trd>
        </trdgrp>
      </snc>
      <trd lng="de">wiedergeben</trd>
      <trdgrp lng="fr">
        <trd>redonner</trd>,
        <trd>rendre<klr tip="ind"> (redonner, reproduire)</klr></trd>,
        <trd>rendre la pareille</trd>,
        <trd>revaloir</trd></trdgrp> 
    </drv>
    <drv mrk="don.trans0i">
      <kap>trans<tld/>i</kap>
      <snc>
        <dif>
          <tld lit="D"/>i al iu ion ricevitan de alia:
          <ekz>
            mi trans<tld/>is al li vian leteron;
          </ekz><ekz>
            trans<tld/>i hereditan bienon.
          </ekz>
        </dif>
      </snc>
      <trd lng="de">&uuml;bergeben</trd>
      <trd lng="fr">transmettre<klr tip="ind"> (donner ce qu'on a re&ccedil;u)</klr></trd>
      <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_d;&c_a;&c_t;&c_mol;</trd>
    </drv>
    <drv mrk="don.ofer0i">
      <kap>ofer<tld/>i</kap>
      <snc>
        <dif>
          Oferi.
        </dif>
      </snc>
      <trd lng="fr">sacrifier</trd>
      <trd lng="ru">&c_p;&c_r;&c_i;&c_n;&c_je;&c_s;&c_t;&c_i; &c_v; &c_zh;&c_je;&c_r;&c_t;&c_v;&c_u;</trd>
    </drv>
    <drv mrk="don.sin0ema">
      <kap>sin<tld/>(em)a</kap>
      <snc>
        <dif>
          Preta servi &ccirc;iuokaze al iu a&ubreve; al io, ne zorgante pri sia
          intereso:
          <ekz>
            ofte amiko estas pli sin<tld/>a ol frato
            <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
              <lok>Sentencoj 18, 24</lok>)</fnt>;
          </ekz><ekz>
            la <ind>sin<tld/>(em)o</ind> de Z al esp-o estas admirinda;
            <trdgrp lng="fr">
              <trd>d&eacute;vouement</trd>,
              <trd>don de soi</trd></trdgrp> 
          </ekz><ekz>
            sin<tld/>a klopodado.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="de"><trd>selbstlos</trd>, <trd>hingebungsvoll</trd></trdgrp>
      <trd lng="fr">d&eacute;vou&eacute;</trd>
      <trdgrp lng="ru">
        <trd>&c_p;&c_r;&c_je;&c_d;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
        <trd>&c_s;&c_a;&c_m;&c_o;&c_o;&c_t;&c_v;&c_je;&c_r;&c_zh;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
      </trdgrp>
    </drv>
  </art>
