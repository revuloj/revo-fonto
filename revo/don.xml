<drv mrk="don.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="don.0i.disponigi">
    <dif>
      Meti en posedon a&ubreve; je dispono de iu, &ccirc;u provizore,
      &ccirc;u definitive:
      <ekz>
        <tld/>i ludilon al infano;
      </ekz><ekz>
        <tld/>i al iu salajron, rekompencon;
      </ekz><ekz>
        <tld/>i al iu por man&gcirc;i kaj trinki;
      </ekz><ekz>
        <tld/>i doton, nomon, garantion, sian vo&ccirc;on, ekzemplon,
        edukon, helpon, favoron, honoron, atenton, konsenton,
        liberecon,
        pruvon;
      </ekz><ekz>
        <tld/>i balon, feston;
      </ekz><ekz>
        la semo <tld/>os fruktojn;
      </ekz><ekz>
        nur senvaloraj objektoj <tld/>is
        <klr>(liveris)</klr> por tiu dekoracio
        la materialon;
      </ekz><ekz>
        <ind><tld/>i al iu la manon</ind>
        <klr>(saluti per manpremo a&ubreve; subteni, apogi)</klr>
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
        <trd lng="de">jemandem die <ind>Hand</ind> reichen</trd>
      </ekz><ekz>
        kolero pravecon ne <tld/>as<fnt><bib>PrV</bib></fnt>;
      </ekz><ekz>
        la &ccirc;ielo <tld/>u, ke...;
      </ekz><ekz>
        &ccirc;ia <ind><tld/>o</ind> estas bono
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="fr">don</trd>
        <trdgrp lng="hu">
          <trd>ad&aacute;s</trd>,
          <trd>adom&aacute;ny</trd>
        </trdgrp>
        <trd lng="pl">dar</trd>
        <trdgrp lng="pt">
          <trd>dar</trd>,
          <trd>entregar</trd>
        </trdgrp>
        <trd lng="ru">&c_d;&c_a;&c_r;</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dispon.0igi">disponigi</ref>,
      <ref cel="donac.0i">donaci</ref>,
      <ref cel="konsen.0i">konsenti</ref>,
      <ref cel="ofer.0i">oferi</ref>
    </refgrp>
  </snc>
  <snc mrk="don.0i.estigi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Estigi, rezultigi, fari agon:
      <ekz>
        &ccirc;u la Kongreso <tld/>is iajn rezultatojn
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la ferma ceremonio de la Sesa Kongreso 
          Esperantista en Washington en la 20a de a&ubreve;gusto 1910 (a)</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la longe dormintaj semoj komencis <ind><tld/>i
        <klr>(produkti)</klr> la unuajn trunketojn</ind>
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Tria Kongreso Esperantista en Cambridge
          en la 12a de a&ubreve;gusto 1907</vrk>
        </fnt>;
        <trdgrp lng="de">
           <trd>austreiben <klr>(Pflanzen)</klr></trd>,
           <trd>sprie&szlig;en</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind><tld/>i <klr>(fari)</klr> signojn per la okuloj</ind>
        <fnt><bib>MT</bib>, <lok>&Sen; 6:13</lok></fnt>;
        <trd lng="de">zu<ind>wzinkern</ind></trd>
      </ekz><ekz>
        <tld/>i baton, vangofrapon;
      </ekz>
      <ekz>
        la patrino <tld/>is al &scirc;i frapon sur la nazon
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i lecionon, ju&gcirc;on, la&ubreve;don, raporton,
        sankcion, punon, instrukcion, ordonon, komision, la tonon;
      </ekz><ekz>
        <tld/>i profiton;
      </ekz><ekz>
        <tld/>i bonan impreson pri;
      </ekz><ekz>
        <tld/>i esperon, kontentigon, anta&ubreve;enpu&scirc;on,
        karakteron de, vidon al;
      </ekz><ekz>
        tiuj formoj <tld/>us grandan
        konfuzon<fnt>Z</fnt>;
      </ekz><ekz>
        tiu odoro <tld/>as kapturni&gcirc;on;
      </ekz><ekz>
        tia elparolado <tld/>as nenian malkompreni&gcirc;on
        <fnt>
          <bib>LR</bib>
          <vrk><url ref="http://eduinf.waw.pl/esp/lern/librejo/0015/0003.php">Pri
          elparolado en teorio kaj praktiko</url></vrk>
          <lok>Respondo 56, Oficiala Gazeto, IV, 1911, p: 222</lok>
        </fnt>;
      </ekz><ekz>
        se simila lingvo e&ccirc; povus esti finita, &gcirc;i nenion
        <tld/>us<fnt>Z</fnt>;
      </ekz><ekz>
        en la <tld/>ita <klr>(difinita, tiama, konsiderata)</klr>
        okazo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pt">
      <trd>dar</trd>,
      <trd>entregar</trd>,
      <trd>aplicar</trd>,
      <trd>ministrar</trd>,
      <trd>administrar</trd>
    </trdgrp>
  </snc>
