<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tuberos.xml,v 1.5 2024/01/25 21:21:01 revo Exp $">
<kap>
  <rad>tuberos</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tuberos.0o">
  <kap><tld/>o</kap>
  <snc mrk="tuberos.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Herbospeco el Meksiko kun blankaj, bonodoraj floroj grapolaj,
      kultivata por parfumfarado,
      specio el familio agavacoj
      (<trd lng="la">Polianthes tuberosa</trd>):
      <ekz>
        <tld/>o kaj ajlo
        <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>&leftquot;soleca baraktado de la viro
        kun la 27a numero surdorse&rightquot;, p. 227a</lok></fnt>;
      </ekz>
      <ekz>
        ili man&gcirc;as la radikan tuberon de <tld/>o, semojn de herboj,
        fruktojn, berojn, insektojn kaj vermojn
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;himalaja monalo&rightquot;,
        2005-08-10</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_b;&c_e;&c_r;&c_o;&c_z;&c_a;</trd>
  <trd lng="fr">tub&eacute;reuse<klr> (subst., esp&egrave;ce)</klr></trd>
  <trd lng="ja">&#x30C1;&#x30E5;&#x30FC;&#x30D9;&#x30ED;&#x30FC;&#x30B9;</trd>
  <trd lng="pl">tuberoza</trd>
  <trd lng="uk">&c_t;&c_u;&c_b;&c_je;&c_r;&#x43E;&c_z;&c_a;</trd>
</drv>
</art>

