<kap>
  <rad>mezalianc</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="mezali.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Ge<ref tip="super" cel="edz.0igxo">edzi&gcirc;o</ref>

 kun persono de malpli alta klaso; misgeedzi&gcirc;o:
      <ekz>
        la <tld/>on ne benis parencaro de la novbakita edzo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=157065">Mi&hcirc;ail Gromov</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        tiam okazis <tld/>o, per kiu Maria intencis pliri&ccirc;i&gcirc;i kaj prosperi en peterburga
        palaco
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=420882">Giacomo Quarenghi</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_e;&c_z;&c_a;&c_l;&c_mol;&c_ja;&c_n;&c_s;</trd>,
    <trd>&c_n;&c_ja;&c_r;&c_o;&c_w;&c_n;&c_y; &c_sh;&c_l;&c_ju;&c_b;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Mesalliance</trd>,
    <trd>Missheirat</trd>,
    <trd>unstandesgem&auml;&szlig;e <ind>Ehe</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>m&eacute;salliance</trd>,
    <trd>rangon aluli <ind>h&aacute;zass&aacute;g</ind></trd>
  </trdgrp>
  <trd lng="ja">&#x4E0D;&#x91E3;&#x308A;&#x5408;&#x3044;&#x306A;&#x7D50;&#x5A5A;  <pr>&#x3075;&#x3064;&#x308A;&#x3042;&#x3044;&#x306A;&#x3051;&#x3063;&#x3053;&#x3093;</pr></trd>
  <trd lng="nl">mesalliance</trd>
  <trd lng="pt">m&eacute;salliance</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_r;&c_a;&c_v;&c_n;&c_y;&c_j; &c_b;&c_r;&c_a;&c_k;</trd>,
