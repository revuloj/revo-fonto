  <rad>parameci</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="parameci.0o">
  <kap><tld/>o</kap>
  <snc mrk="parameci.0o.BAK">
    <uzo tip="fak">BAK</uzo>
    <dif>
      Cilia protozoo;
      genro el familio parameciedoj
      (<trd lng="la">Paramecium</trd>):
      <ekz>
        observadi la vivon de <tld/>oj kaj ameboj
        sub mikroskopo
        <fnt>
          <aut>Ba Jin, trad. Li Shijun</aut>,
          <vrk>A&ubreve;tuno en la Printempo</vrk>,
          <lok>1980</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">param&eacute;cie</trd>
  <trd lng="id">paramecium</trd>
  <trd lng="pl">pantofelek  <klr>(pierwotniak)</klr></trd>
  <trd lng="ro">parameci</trd>
</drv>
</art>

