<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: parameci.xml,v 1.5 2022/10/28 08:46:53 revo Exp $">
<kap>
  <rad>parameci</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="parameci.0o">
  <kap><tld/>o</kap>
  <snc mrk="parameci.0o.BAK">
    <uzo tip="fak">BAK</uzo>
    <dif>
      Cilia protozoo;
      genro el familio parameciedoj
      (<trd lng="la">Paramecium</trd>):
      <ekz>
        observadi la vivon de <tld/>oj kaj ameboj
        sub mikroskopo
        <fnt>
          <aut>Ba Jin, trad. Li Shijun</aut>,
          <vrk>A&ubreve;tuno en la Printempo</vrk>,
          <lok>1980</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_f;&c_u;&c_z;&c_o;&c_r;&c_y;&c_ja;-&c_t;&c_u;&c_f;&c_je;&c_l;&c_mol;&c_k;&c_a;</trd>
  <trd lng="fr">param&eacute;cie</trd>
  <trd lng="id">paramecium</trd>
  <trd lng="pl">pantofelek  <klr>(pierwotniak)</klr></trd>
  <trd lng="ro">parameci</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_u;&c_f;&c_je;&#x43B;&#x44C;&#x43A;&c_a;</trd>,
    <trd>&#x43F;&c_a;&c_r;&c_a;&#x43C;&c_je;&c_c;&c_ib;&#x44F;</trd>
  </trdgrp>
</drv>
</art>

