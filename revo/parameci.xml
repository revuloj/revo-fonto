<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: parameci.xml,v 1.6 2024/01/31 21:20:31 revo Exp $">
<kap>
  <rad>parameci</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="parameci.0o">
  <kap><tld/>o</kap>
  <snc mrk="parameci.0o.BAK">
    <uzo tip="fak">BAK</uzo>
    <dif>
      Cilia <ref tip="super" cel="protoz.0o">protozoo</ref>;
      genro el familio parameciedoj (<trd lng="la">Paramecium</trd>):
      <ekz>
        observadi la vivon de <tld/>oj kaj ameboj
        sub mikroskopo
        <fnt>
          <aut>Ba Jin, trad. Li Shijun</aut>,
          <vrk>A&ubreve;tuno en la Printempo</vrk>,
          <lok>1980</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/c/cb/Paramecium.jpg/244px-Paramecium.jpg"
      prm="CC BY-SA 3.0"
      ><tld/>o (Paramecium aurelia), la &ccirc;irka&ubreve;aj cilioj neklaraj pro rapida movi&gcirc;o
        <fnt>
          <aut>Barfooz</aut>
          <lok><url ref="&WCW;/File:Paramecium.jpg"
              >https://commons.wikimedia.org/wiki/File:Paramecium.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_f;&c_u;&c_z;&c_o;&c_r;&c_y;&c_ja;-&c_t;&c_u;&c_f;&c_je;&c_l;&c_mol;&c_k;&c_a;</trd>
  <trd lng="de">Pantoffeltierchen</trd>
  <trd lng="fr">param&eacute;cie</trd>
  <trd lng="id">paramecium</trd>
  <trd lng="pl">pantofelek  <klr>(pierwotniak)</klr></trd>
  <trd lng="ro">parameci</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_u;&c_f;&c_je;&c_l;&c_mol;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_a;&c_m;&c_je;&c_c;&c_ib;&c_ja;</trd>
  </trdgrp>
</drv>
</art>

