<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: konkur1.xml,v 1.26 2024/03/19 12:39:06 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>konkurenc</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="konkur1.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="konkur1.0o.EKON">
  <uzo tip="fak">EKON</uzo>
    <dif>
      Komerca <ref tip="super" cel="konkur.0ado">konkurado</ref>:
      <ekz>
        formado de monopoloj <klr>[&#8230;]</klr> eliminas <tld/>on
        <fnt>
       <aut>Gilbert Ledon</aut>
          <vrk><url ref="&Monato;/2000/006861.php">Milito kaj paco</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 5</lok>
        </fnt>;
      </ekz>
      <ekz>
        pro manko de <tld/>o, la &Ccirc;ina Civila Aviado povas mem la&ubreve;dezire fiksi a&ubreve;
        plialtigi la prezojn
        <fnt>
          <aut>MU Binghua</aut>
            <vrk><url ref="&Monato;/2000/007047.php">Enlanda aviado multekostas</url></vrk>
            <bib>Monato</bib>
            <lok>2000/08, p. 10</lok>
          </fnt>;
      </ekz>
      <ekz>
        ke kompanioj estu <tld/>kapablaj, pri tio e&ccirc; la &scirc;tato atentas
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 31</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>on inter gentoj ili tuj profitis, kaj akrigis la disputojn per agentoj
        specialaj
        <fnt>
          <bib>VivZam</bib>
          <lok>Vivo de Zamenhof</lok>
        </fnt>;
      </ekz>
      <ekz>
        la rezultoj de <tld/>aj investaj ebloj estis bonegaj: <klr>[&#8230;]</klr> la dolara
        regiono havis aparte bonajn rezultojn
        <fnt>
	<aut>Marc Vanden Bempt</aut>
	<vrk><url ref="&Monato;/2000/006821.php">E&ubreve;ro unujara</url></vrk>
	<bib>Monato</bib>
	<lok>2000/03, p. 14</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konkur.0eco">konkureco</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_k;&c_u;&c_r;&c_e;&c_n;&c_c;&c_y;&c_ja;</trd>
  <trd lng="ca">compet&egrave;ncia<klr> (econ.)</klr></trd>
  <trdgrp lng="de">
     <trd>Konkurrenz</trd>,
     <trd>Wettbewerb <klr>(Kommerz)</klr></trd>
  </trdgrp>
  <trd lng="en">commercial <ind>competition</ind></trd>
  <trd lng="es">competencia</trd>
  <trd lng="fr">concurrence</trd>
  <trdgrp lng="ja">
    <trd>&#x7AF6;&#x4E89;  <pr>&#x304D;&#12423;&#12358;&#x305D;&#12358;</pr></trd>,
    <trd>&#x7AF6;&#21512;  <pr>&#x304D;&#12423;&#12358;&#12372;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">concurrentie</trd>
  <trd lng="pl">konkurencja <klr> (ekonomia)</klr></trd>
  <trd lng="pt">concorr&ecirc;ncia</trd>
  <trd lng="ro">concuren&#x21B;&abreve;</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_je;&c_n;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#x7ADE;&#x4E89; <pr>j&#xEC;ngzh&emacron;ng</pr></trd>,
    <trd>&#x5E02;&#x573A;&#x7ADE;&#x4E89; <pr>sh&#xEC;ch&#x1CE;ngj&#xEC;ngzh&emacron;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="konkur1.0i">
  <kap>
    <tld/>i
  </kap>
  <snc mrk="konkur1.0i.ekonomie">
    <dif>
      Ekonomie <ref tip="super" cel="konkur.0i">konkuri</ref>:
      <ekz>
        komercistoj nur <tld/>is
        <fnt>
          <bib>VivZam</bib>
          <lok>Vivo de Zamenhof</lok>
        </fnt>;
      </ekz>
      <ekz>
        estas tre malfacile <tld/>i kun grandaj kompanioj
        <fnt>
       <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2000/006883.php">Fulma kolero</url></vrk>
          <bib>Monato</bib>
          <lok>2000/09, p. 14</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en la liverpulaj marist-drinkejoj mi ne trovis tian obscenbu&scirc;an velmajstron, kiu
        povus <tld/>i kun la &leftquot;pli &ccirc;arma&rightquot; sekso de la hinoj
        <fnt>
          <bib>VaK</bib>
          <lok>Sepa &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_n;&c_k;&c_u;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_b;&c_o;&c_r;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol; <klr>(&c_e;&c_k;&c_a;&c_n;&c_a;&c_m;&c_ib;&c_ch;&c_n;&c_a;)</klr></trd>,
    <trd>&c_s;&c_t;&c_v;&c_a;&c_r;&c_a;&c_c;&c_mol; <ind>&c_k;&c_a;&c_n;&c_k;&c_u;&c_r;&c_e;&c_n;&c_c;&c_y;&c_ju;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>konkurrieren</trd>,
    <trd>in <ind>Wettberwerb</ind> stehen</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7AF6;&#x4E89;&#12377;&#x308B;  <pr>&#x304D;&#12423;&#12358;&#x305D;&#12358;&#12377;&#x308B;</pr></trd>,
    <trd>&#x7AF6;&#21512;&#12377;&#x308B;  <pr>&#x304D;&#12423;&#12358;&#12372;&#12358;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x647D; <pr>bi&#xE0;o</pr></trd>,
    <trd>&#x7ADE;&#x4E89;&#24615; <pr>j&#xEC;ngzh&emacron;ngx&#xEC;ng</pr></trd>,
    <trd>&#x7ADE;&#x4E89; <pr>j&#xEC;ngzh&emacron;ng</pr></trd>
  </trdgrp>
</drv>

</art>
