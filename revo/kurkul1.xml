<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kurkul1.xml,v 1.15 2023/12/03 09:59:04 revo Exp $">
<kap>
  <rad>kurkulied</rad>/oj
</kap>
<drv mrk="kurkul1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> 
      el la ordo <ref tip="malprt" cel="koleop.0oj">koleopteroj</ref>
      (<trd lng="la">Curculionidae</trd>), kun rostre longigita kapo:
      <ekz>
        kameropso <klr>[&#x2026;]</klr> floras en aprilo-majo kaj estas polenata de specifa <tld/>o, 
        <nom>Derelomus chamaeropsis</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=454744">Kameropso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
       <ref cel="kurkul.0o">kurkulio</ref>,
       <ref cel="skarab.gren0o">grenskarabo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_l;&c_o;&c_n;&c_ib;&c_k;&c_ib; <klr>(&c_s;&c_ja;&c_m;&#x2019;&c_ja; &c_zh;&c_u;&c_k;&c_o;&c_w;)</klr></trd>
  <trd lng="ca">curculi&ograve;nids</trd>
  <trd lng="de">R&uuml;sselk&auml;fer</trd>
  <trd lng="fr">Curculionid&eacute;s</trd>
  <trd lng="hu">orm&aacute;nyos <ind>bogarak</ind></trd>
  <trd lng="ja">&#x30BE;&#x30A6;&#x30E0;&#x30B7;</trd>
  <trdgrp lng="nl">
    <trd>snuittorren<klr tip="ind"> (fam.)</klr></trd>,
    <trd>snuitkevers<klr tip="ind"> (fam.)</klr></trd>
  </trdgrp>
  <trd lng="pl">ryjkowcowate</trd>     
  <trd lng="ro">curculionide</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_l;&c_g;&c_o;&c_n;&c_o;&c_s;&c_i;&c_k;&c_i;<klr tip="ind"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_s;&c_l;&c_o;&c_n;&c_i;&c_k;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="uk">&c_d;&#x43E;&c_v;&c_g;&#x43E;&#x43D;&#x43E;&c_s;&c_i;&#x43A;&#x43E;&c_v;&c_ib;</trd>
  <trdgrp lng="zh">
    <trd>&#x8C61;&#x9F3B;&#x866B; <pr>xi&#xE0;ngb&#xED;ch&#xF3;ng</pr></trd>,
    <trd>&#x8C61;&#x9F3B;&#x87F2; <pr>xi&#xE0;ngb&#xED;ch&#xF3;ng</pr></trd>
  </trdgrp>
</drv>
</art>
