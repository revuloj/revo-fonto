<drv mrk="organi2.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="organi2.0i.POL">
    <dif>
      <ref tip="super" cel="star.0igi.fondi">Starigi</ref> ion, kunordigante &gcirc;iajn partojn por
      plenumi difinitan
      funkcion; doni al io la necesajn <ref tip="prt" cel="organ.0o">organo</ref> por funkciadi:
      <ekz>
        en la jaro 1884 la societo <tld/>is la unuan specialan fakultaton
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        U.E.A. <klr>[&#x2026;]</klr> reprezentas nur personojn, kiuj deziras esti <tld/>ataj
        pro difinita celo aprobita de ili
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la malferma kunveno de UEA &ccirc;e la Kvina Kongreso Esperantista en
          Barcelona en la 7a de septembro 1909</vrk>
        </fnt>;
      </ekz>
      <ekz>
        koran dankon al la <tld/>intoj de la nuna kongreso
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Dua Kongreso Esperantista en Gen&egrave;ve en la 28a de a&ubreve;gusto 1906</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;irka&ubreve;a &scirc;ovinismo timigis la parizajn <tld/>antojn, ili petegis <nom>Zamenhof</nom> ne
        kunvoki tian kunvenon, e&ccirc; privatan, dum la Kongreso
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        plenforme <tld/>ita <klr>[&#x2026;]</klr> komunumo de liberkredantoj
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Homaranismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la Laboristaro devas sin <tld/>i mondskale
        <fnt>
          <bib>Lanti</bib>
        </fnt>;
      </ekz>
      <ekz>
        ni devas lerni kaj labori sisteme kaj <tld/>ite
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is du avangardajn liniojn, el kiuj la pli malproksima devis observi la
        malamikojn, la pli proksima &dash; gardi la propran armeon kontra&ubreve; atako
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        la man&gcirc;aj oficistoj venos <klr>[&#x2026;]</klr> por <tld/>i sub tendo, starigita por
        tiu uzo, posttagman&gcirc;on tute allogantan
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i tie estis &ccirc;io en ia trankviliga, <tld/>ita malordo
        <fnt>
          <bib>KKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        turistoj al &gcirc;i iras are, ili tamen <tld/>is por tiu mateno karavanon, kiun mi
        kondukos
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        lingvo <klr>[&#x2026;]</klr> <tld/>ita kiel sistemo de simboloj, reprezentantaj ideojn
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        cedi al esta&jcirc;o pli alte <tld/>ita
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="arangx.0i">aran&gcirc;i</ref>,
      <ref cel="establ.0i">establi</ref>,
      <ref cel="instal.0i">instali</ref>,
      <ref cel="kompon.0i.kunmeti_konsistigajn_elementoj">komponi<sncref/></ref>,
      <ref cel="ord.0igi">ordigi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bo">&t_ga;&t_rata;&t_tsheg;&t_-a;&t_ga;&t_rata;&t_gigu;&t_ga;&t_sa;&t_tsheg;&t_ba;&t_yata;&t_drengbu;&t_da;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trdgrp lng="cs">
