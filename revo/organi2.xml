<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>organiz</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="organi2.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="organi2.0i.POL">
    <dif>
      Starigi ion, kunordigante &gcirc;iajn partojn por plenumi difinitan
      funkcion; doni al io la necesajn organojn por funkciadi:
      <ekz>
        <tld/>i armeon, &scirc;taton, kurson, ekskurson, oficejon, societon,
        kongreson;
      </ekz>
      <ekz>
        <tld/>ata memlernado<fnt>Z</fnt>;
      </ekz>
      <ekz>
        esta&jcirc;o alte <tld/>ita.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="arangx.0i">aran&gcirc;i</ref>,
      <ref cel="establ.0i">establi</ref>,
      <ref cel="instal.0i">instali</ref>,
      <ref cel="ord.0igi">ordigi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">organisieren</trd>
  <trd lng="en">organize</trd>
  <trd lng="es">organizar</trd>
  <trdgrp lng="fr">
    <trd>disposer</trd>,
    <trd>organiser</trd>
  </trdgrp>
  <trd lng="hu">szervez</trd>
  <trd lng="nl">organiseren</trd>
  <trd lng="pt">organizar</trd>
  <trd lng="ru">&c_o;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="organi2.0o">
  <kap><tld/>o</kap>
  <snc mrk="organi2.0o.POL">
    <dif>
      Stato de tuto ordigita por plenumado de difinita funkcio:
      <ekz>
        la <tld/>o de la societo, de tribunaloj, de la Lingvaj
        Institucioj
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aparat.0o">aparato</ref>,
      <ref cel="serv.0o">servo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ja; <klr>(&c_u;&c_p;&c_a;&c_r;&c_a;&c_d;&c_k;&c_a;&c_v;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;,
  &c_s;&c_t;&c_r;&c_u;&c_k;&c_t;&c_u;&c_r;&c_a;)</klr></trd>
  <trd lng="de">Organisation</trd>
  <trd lng="en">organization</trd>
  <trd lng="es">organizaci&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>configuration</trd>,
    <trd>disposition</trd>,
    <trd>organisation</trd>
  </trdgrp>
  <trd lng="hu">szervezet</trd>
  <trd lng="nl">organisatie</trd>
  <trd lng="pt">organiza&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_o;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;</trd>
</drv>

<drv mrk="organi2.0ado">
  <kap><tld/>ado, <var><kap><tld/>o</kap></var></kap>
  <snc mrk="organi2.0ado.POL">
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ja; <klr>(&c_d;&c_z;&c_je;&c_ja;&c_n;&c_mol;&c_n;&c_je;)</klr></trd>
  <trd lng="de">Organisieren</trd>
  <trd lng="es">organizaci&oacute;n</trd>
  <trd lng="fr">organisation<klr> (action)</klr></trd>
  <trd lng="hu">szervez&eacute;s</trd>
  <trd lng="nl">organiseren</trd>
  <trd lng="pt">organiza&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_o;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja; <klr>(&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;)</klr></trd>
</drv>

<drv mrk="organi2.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="organi2.0ajxo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Io <tld/>ita:
      <ekz>
        jam li fondis plurajn bankajn <tld/>a&jcirc;ojn;
      </ekz>
      <ekz>
        la <tld/>a&jcirc;o estas bazita sur la principo de suverena egaleco.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="asoci.0o">asocio</ref>,
      <ref cel="lig.0o.asocio">ligo</ref>,
      <ref cel="societ.0o">societo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_u;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Organisation</trd>
  <trd lng="es">organizaci&oacute;n</trd>
  <trd lng="fr">organisation<klr> (institution)</klr></trd>
  <trd lng="hu">szervezet</trd>
  <trd lng="nl">organisatie</trd>
  <trd lng="pt">organiza&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_o;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;</trd>
</drv>

<drv mrk="organi2.neregistara0ajxo">
  <kap>neregistara <tld/>a&jcirc;o</kap>
  <mlg>NRO</mlg>
  <snc mrk="organi2.neregistara0ajxo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <tld/>a&jcirc;o, asocio, kiu agas je internacia skalo
      sen dependo je registaro a&ubreve; &scirc;tato,
      precipe iu el tiuj, kiuj ne celas profiton,
      sed bonfaron, socian progreson...:
      <ekz>
        la internacia le&gcirc;igo de la homaj rajtoj estas rezulto de 
        longaj, insistaj klopodoj, en kiuj elstaran rolon ludis influaj 
        unuopuloj, diversaj premgrupoj kaj neregistaraj <tld/>a&jcirc;oj
        <fnt>
          <aut>I. Lapenna</aut>,
          <vrk><url
          ref="http://www.ivolapenna.org/historiegravaj/manifesto_100107.htm">
          Manifesto 77 de la Esperanto&#173;-Movado</url></vrk>,
          <lok>Horizonto, 1977:8</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">organisation non gouvernementale</trd>
</drv>
</art>
