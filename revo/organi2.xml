  <trdgrp lng="ja">
    <trd>&#x7D44;&#x7E54;&#x3059;&#x308B;  <pr>&#x305D;&#x3057;&#x304D;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x8A2D;&#x7ACB;&#x3059;&#x308B;  <pr>&#x305B;&#x3064;&#x308A;&#x3064;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x6E96;&#x5099;&#x3059;&#x308B;  <pr>&#x3058;&#x3085;&#x3093;&#x3073;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x50AC;&#x3059;  <pr>&#x3082;&#x3088;&#x304A;&#x3059;</pr></trd>
  </trdgrp>
  <trd lng="nl">organiseren</trd>
  <trdgrp lng="pl">
    <trd>organizowa&cacute;</trd>,
    <trd>urz&aogonek;dzi&cacute;</trd>,
    <trd>zorganizowa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">organizar</trd>
  <trdgrp lng="ro">
    <trd>organiza</trd>,
    <trd>sorta</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>organizova&tcaron;</trd>,
    <trd>usporiada&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_o;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_o;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x7D44;&#x7E54; <pr>z&#x1D4;zh&#x12B;</pr></trd>,
    <trd>&#x7EC4;&#x7EC7; <pr>z&#x1D4;zh&#x12B;</pr></trd>,
    <trd>&#x7D44; <pr>z&#x1D4;</pr></trd>,
    <trd>&#x7EC4; <pr>z&#x1D4;</pr></trd>,
    <trd>&#x4E3E;&#x529E; <pr>j&#x1D4;b&#xE0;n</pr></trd>,
    <trd>&#x8209;&#x8FA6; <pr>j&#x1D4;b&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="organi2.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      <tld lit="O"/>anta, kies tasko estas <tld/>i ion; rilata al <tld/>ado:
      <ekz>
        la loka <tld/>a komitato laboris por <klr>[&#x2026;]</klr> preparado de nia nuna
        kongreso
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvina Kongreso Esperantista en Barcelona en la 6a de septembro
          1909</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a laboro
        <fnt>
          <bib>AzS</bib>
        </fnt>;
      </ekz>
      <ekz>
        aktualaj <tld/>aj problemoj, demandoj, diskutoj
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a strukturo, skemo, kadro, fundamento, reformo
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj principoj, formoj, planoj
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ni]</klr> neniam povos forgesi lian sindonan kaj sukcesan laboradon
        <klr>[kaj]</klr> lian grandan <tld/>an talenton
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        la universaleco de la Lingvo trovis adekvatan <tld/>an formon en nia Universala
        Esperanto-Asocio
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        tio reflekti&gcirc;as en la Esperanto-Movado je la niveloj lingva, kultura kaj <tld/>a
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        Esperanto-asocioj funkciis kiel <tld/>aj centroj de interlerneja korespondado
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        jam en 1908 oni komencis rimarki la <tld/>an kaj ideologian dividon inter la
        bur&gcirc;a kaj proleta esperantista Movado
        <fnt>
          <bib>EeP</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_j;&c_n;&c_y;</trd>
  <trd lng="ja">&#x7D44;&#x7E54;&#x306E;  <pr>&#x305D;&#x3057;&#x304D;&#x306E;</pr></trd>
  <trdgrp lng="pl">
    <trd>organizacyjny</trd>,
    <trd>zarz&aogonek;dzaj&aogonek;cy</trd>
  </trdgrp>
  <trd lng="ro">gestion&acirc;nd</trd>
</drv>

<drv mrk="organi2.0o">
  <kap><tld/>o</kap>
  <snc mrk="organi2.0o.ago">
    <dif>
      Ago <tld/>i ion, <ref tip="dif" cel="organi2.0ado">organizado</ref>:
      <ekz>
        <nom>Pentuer</nom> estis okupita per la <tld/>o de la kuracista helpo por la vunditoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="organi2.0o.maniero">
    <dif>
      <ref tip="super" cel="manier.0o.KOMUNE">Maniero</ref> kiel homoj estas <tld/>itaj:
      <ekz>
        Egipto jam posedis altan socian <tld/>on, terkulturon, metiojn kaj literaturon
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiuj gigantaj laboroj postulis, krom konoj de la astronomio, geometrio, me&hcirc;aniko
        kaj ar&hcirc;itekturo, anka&ubreve; perfektan <tld/>on
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li fondis grupon, kiu per sia bonega <tld/>o kaj vigla laborado balda&ubreve; fari&gcirc;is
        imitinda modelo
        <fnt>
          <vrk><url ref="&Tekstaro;?nomo=paroladoj-de-zamenhof">Parolado super la Tombo
          de Karlo Bourlet, 1913</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li ree, sub la preteksto studi la <tld/>on de la diman&ccirc;aj lernejoj, trotis en la
        urbon
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>;
      </ekz>
      <ekz>
        la revolucio ankora&ubreve; ne povis montri <tld/>on
        <fnt>
          <bib>LGA</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aparat.0o">aparato</ref>,
      <ref cel="serv.0o">servo</ref>
    </refgrp>
  </snc>
  <snc mrk="organi2.0o.ajxo">
    <dif>
      <ref tip="dif" cel="organi2.0ajxo"><tld lit="O"/>a&jcirc;o</ref>:
      <ekz>
        gvidantoj de partioj a&ubreve; <tld/>oj laboristaj
        <fnt>
          <bib>Lanti</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiel SAT-ano, li ja kelkfoje alvenis al konkludoj, kiuj ne tute koincidas kun
        tiuj de politikaj <tld/>oj
        <fnt>
          <bib>Lanti</bib>
        </fnt>;
      </ekz>
      <ekz>
        li dum la jaroj 1944 kaj 1945 gvidis <klr>[&#x2026;]</klr> vastskalan <tld/>on je la
        nomo de Sveda Ru&gcirc;a Kruco
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
        </fnt>;
      </ekz>
      <ekz>
        membro de loka <tld/>o fari&gcirc;as gvidanto en federacio
        <fnt>
          <bib>ViV</bib>
        </fnt>;
      </ekz>
      <ekz>
        memoru, ke vi estas parto de granda <tld/>o por helpi al la homaro
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        Lu&ccirc;ia-festo <klr>[estas]</klr> solenata en &ccirc;iuj familioj, en &ccirc;iuj <tld/>oj,
        korporacioj
        <fnt>
          <bib>KKr</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ja; <klr>(&c_u;&c_p;&c_a;&c_r;&c_a;&c_d;&c_k;&c_a;&c_v;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;, &c_s;&c_t;&c_r;&c_u;&c_k;&c_t;&c_u;&c_r;&c_a;)</klr></trd>
  <trdgrp lng="cs">
    <trd>organizace</trd>,
    <trd>organizov&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Organisation</trd>
  <trd lng="en">organization</trd>
  <trd lng="es">organizaci&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>configuration</trd>,
    <trd>disposition</trd>,
    <trd>organisation</trd>
  </trdgrp>
  <trd lng="hu">szervezet</trd>
  <trd lng="id">organisasi</trd>
  <trdgrp lng="ja">
    <trd>&#x7D44;&#x7E54;  <pr>&#x305D;&#x3057;&#x304D;</pr></trd>,
    <trd>&#x6A5F;&#x69CB;  <pr>&#x304D;&#x3053;&#x3046;</pr></trd>,
    <trd>&#x56E3;&#x4F53;  <pr>&#x3060;&#x3093;&#x305F;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">organisatie</trd>
  <trd lng="pl">organizowanie</trd>
  <trd lng="pt">organiza&ccedil;&atilde;o</trd>
  <trd lng="ro">organizare</trd>
  <trd lng="ru">&c_o;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>organizovanie</trd>,
    <trd>organiz&aacute;cia</trd>
  </trdgrp>
  <trd lng="uk">&c_o;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_c;&c_ib;&c_ja; (&c_o;&c_r;&c_g;&c_a;&c_n;)</trd>
  <trdgrp lng="zh">
    <trd>&#x673A;&#x6784; <pr>j&#x12B;g&#xF2;u</pr></trd>,
    <trd>&#x6A5F;&#x69CB; <pr>j&#x12B;g&#xF2;u</pr></trd>,
    <trd>&#x56E2;&#x4F53; <pr>tu&#xE1;nt&#x1D0;</pr></trd>,
    <trd>&#x5718;&#x9AD4; <pr>tu&#xE1;nt&#x1D0;</pr></trd>,
    <trd>&#x793E;&#x56E2; <pr>sh&#xE8;tu&#xE1;n</pr></trd>,
    <trd>&#x793E;&#x5718; <pr>sh&#xE8;tu&#xE1;n</pr></trd>,
    <trd>&#x7D44;&#x7E54; <pr>z&#x1D4;zh&#x12B;</pr></trd>,
    <trd>&#x7EC4;&#x7EC7; <pr>z&#x1D4;zh&#x12B;</pr></trd>,
    <trd>&#x7D44;&#x5718; <pr>z&#x1D4;tu&#xE1;n</pr></trd>,
    <trd>&#x7EC4;&#x56E2; <pr>z&#x1D4;tu&#xE1;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="organi2.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        li zorgis pri &ccirc;io, tempon kaj volon li donis por la <tld/>ado de nia grava festo
        <fnt>
          <vrk><url ref="&Tekstaro;?nomo=paroladoj-de-zamenhof">Oficiala Parolado &ccirc;e la
          Festeno</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li pritraktis <klr>[&#x2026;]</klr> demandojn pri <tld/>ado kaj pri interlingvistiko
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        Soveta Esperantista Junulara Movado <klr>[&#x2026;]</klr> transprenis i.a. la
        <tld/>adon de la tradiciaj Junularaj Tendaroj
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> interesi&gcirc;as pri sovetia publiksaneca organizado
        <fnt>
          <bib>CKv</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ja; <klr>(&c_d;&c_z;&c_je;&c_ja;&c_n;&c_mol;&c_n;&c_je;)</klr></trd>
