<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: organi2.xml,v 1.23 2021/05/28 12:53:05 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>organiz</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="organi2.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="organi2.0i.POL">
    <dif>
      Starigi ion, kunordigante &gcirc;iajn partojn por plenumi difinitan
      funkcion; doni al io la necesajn organojn por funkciadi:
      <ekz>
        <tld/>i armeon, &scirc;taton, kurson, ekskurson, oficejon, societon,
        kongreson;
      </ekz>
      <ekz>
        <tld/>ata memlernado<fnt>Z</fnt>;
      </ekz>
      <ekz>
        esta&jcirc;o alte <tld/>ita.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="arangx.0i">aran&gcirc;i</ref>,
      <ref cel="establ.0i">establi</ref>,
      <ref cel="instal.0i">instali</ref>,
      <ref cel="kompon.0i.kunmeti_konsistigajn_elementoj">komponi<sncref/></ref>,
      <ref cel="ord.0igi">ordigi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bo">&#xF42;&#xFB2;&#xF0B;&#xF60;&#xF42;&#xFB2;&#xF72;&#xF42;&#xF66;&#xF0B;&#xF56;&#xFB1;&#xF7A;&#xF51;&#xF0B;&#xF54;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>uspo&rcaron;&aacute;dat</trd>,
    <trd>zorganizovat</trd>
  </trdgrp>
  <trd lng="de">organisieren</trd>
  <trd lng="en">organize</trd>
  <trd lng="es">organizar</trd>
  <trdgrp lng="fr">
    <trd>disposer</trd>,
    <trd>organiser</trd>
  </trdgrp>
  <trd lng="hu">szervez</trd>
  <trd lng="id">meng<ind>organisasi</ind></trd>
  <trd lng="nl">organiseren</trd>
  <trd lng="pt">organizar</trd>
  <trd lng="ru">&c_o;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>organizova&tcaron;</trd>,
    <trd>usporiada&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="organi2.0o">
  <kap><tld/>o</kap>
  <snc mrk="organi2.0o.POL">
    <dif>
      Stato de tuto ordigita por plenumado de difinita funkcio:
      <ekz>
        la <tld/>o de la societo, de tribunaloj, de la Lingvaj
        Institucioj
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aparat.0o">aparato</ref>,
      <ref cel="serv.0o">servo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ja; <klr>(&c_u;&c_p;&c_a;&c_r;&c_a;&c_d;&c_k;&c_a;&c_v;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;,
  &c_s;&c_t;&c_r;&c_u;&c_k;&c_t;&c_u;&c_r;&c_a;)</klr></trd>
  <trdgrp lng="cs">
    <trd>organizace</trd>,
    <trd>organizov&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Organisation</trd>
  <trd lng="en">organization</trd>
  <trd lng="es">organizaci&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>configuration</trd>,
    <trd>disposition</trd>,
    <trd>organisation</trd>
  </trdgrp>
  <trd lng="hu">szervezet</trd>
  <trd lng="id">organisasi</trd>
  <trd lng="nl">organisatie</trd>
  <trd lng="pt">organiza&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_o;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>organizovanie</trd>,
    <trd>organiz&aacute;cia</trd>
  </trdgrp>
</drv>

<drv mrk="organi2.0ado">
  <kap><tld/>ado, <var><kap><tld/>o</kap></var></kap>
  <snc mrk="organi2.0ado.POL">
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ja; <klr>(&c_d;&c_z;&c_je;&c_ja;&c_n;&c_mol;&c_n;&c_je;)</klr></trd>
  <trd lng="bo">&#xF66;&#xF92;&#xFB2;&#xF72;&#xF42;&#xF0B;&#xF60;&#xF5B;&#xF74;&#xF42;&#xF66;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>organizace</trd>,
    <trd>organizov&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Organisieren</trd>
  <trd lng="es">organizaci&oacute;n</trd>
  <trd lng="fr">organisation<klr> (action)</klr></trd>
  <trd lng="hu">szervez&eacute;s</trd>
  <trd lng="id">organisasi</trd>
  <trd lng="nl">organiseren</trd>
  <trd lng="pt">organiza&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_o;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja; <klr>(&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;)</klr></trd>
  <trdgrp lng="sk">
    <trd>organizovanie</trd>,
    <trd>organiz&aacute;cia</trd>
  </trdgrp>
</drv>

<drv mrk="organi2.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="organi2.0ajxo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Io <tld/>ita:
      <ekz>
        jam li fondis plurajn bankajn <tld/>a&jcirc;ojn;
      </ekz>
      <ekz>
        la <tld/>a&jcirc;o estas bazita sur la principo de suverena egaleco.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="asoci.0o">asocio</ref>,
      <ref cel="lig.0o.asocio">ligo</ref>,
      <ref cel="societ.0o">societo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_u;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&#xF5A;&#xF7C;&#xF42;&#xF66;&#xF0B;&#xF54;&#xF0B;</trd>
  <trd lng="de">Organisation</trd>
  <trd lng="es">organizaci&oacute;n</trd>
  <trd lng="fr">organisation<klr> (institution)</klr></trd>
  <trd lng="hu">szervezet</trd>
  <trd lng="id">organisasi</trd>
  <trd lng="nl">organisatie</trd>
  <trd lng="pt">organiza&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_o;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;</trd>
</drv>

<drv mrk="organi2.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="organi2.0igxi.KOMUNE">
    <dif>
      Kunaran&gcirc;i la funkciojn de siaj diversaj
      partoj:
      <ekz>
        la kompanio iel sukcesis <tld/>i&gcirc;i kiel longa,
        malregula kolumno
        <fnt><aut>A. L&ouml;wenstein</aut>, <vrk>La &scirc;tona urbo</vrk>, 
        <lok>1999</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_o;&c_w;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trd lng="fr">s'<ind>organiser</ind></trd>
  <trd lng="id">ter<ind>organisasi</ind></trd>
</drv>

<drv mrk="organi2.dis0igxi">
  <kap>dis<tld/>i&gcirc;i,
  <var><kap>mal<tld/>i&gcirc;i</kap></var></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="organi2.dis0igxi.KOMUNE">
    <dif>
      Perdi sian bonan, komunan funkciadon:
      <ekz>
        la eldonejo dis<tld/>i&gcirc;is pro perdo de la financa subtenanto
        <fnt>
          <aut>Hasegawa T.</aut>,
          <vrk><url ref="http://wikilivres.ca/wiki/En_%C4%88inio_Batalanta">
          En &Ccirc;inio Batalanta</url></vrk>, 
          <lok>1954</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fal.dis0i">disfali</ref>,
      <ref cel="kompon.mal0igxi">malkomponi&gcirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="fr">se <ind>d&eacute;sorganiser</ind></trd>
</drv>

<drv mrk="organi2.neregistara0ajxo">
  <kap>neregistara <tld/>a&jcirc;o</kap>
  <mlg>NRO</mlg>
  <snc mrk="organi2.neregistara0ajxo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <tld/>a&jcirc;o, asocio, kiu agas je internacia skalo
      sen dependo je registaro a&ubreve; &scirc;tato,
      precipe iu el tiuj, kiuj ne celas profiton,
      sed bonfaron, socian progreson...:
      <ekz>
        la internacia le&gcirc;igo de la homaj rajtoj estas rezulto de 
        longaj, insistaj klopodoj, en kiuj elstaran rolon ludis influaj 
        unuopuloj, diversaj premgrupoj kaj neregistaraj <tld/>a&jcirc;oj
        <fnt>
          <aut>I. Lapenna</aut>,
          <vrk><url ref="http://www.ivolapenna.org/historiegravaj/manifesto_100107.htm">
          Manifesto 77 de la Esperanto&#xAD;-Movado</url></vrk>,
          <lok>Horizonto, 1977:8</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_ja;&c_w;&c_r;&c_a;&c_d;&c_a;&c_v;&c_a;&c_ja; &c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trd lng="fr">organisation non gouvernementale</trd>
  <trd lng="id"><ind>organisasi</ind> nonpemerintah</trd>
</drv>

<drv mrk="organi2.Internacia_Labor0ajxo">
  <kap>Internacia Labor-<tld lit="O"/>a&jcirc;o</kap>
  <mlg>ILO</mlg>
  <fnt><bib>ManPol</bib></fnt>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Speciala <ref tip="lst" cel="organi2.0ajxo" lst="voko:organiza&jcirc;oj"><tld/>a&jcirc;o</ref> de
      <ref tip="malprt" cel="naci.Unuigxintaj_0oj">Unui&gcirc;intaj Nacioj</ref>,
      kiu celas akceli la socialan justecon per plibonigoj de la viv-
      kaj labor-kondi&ccirc;oj:
      <ekz>
        spertuloj de la Internacia Labor-<tld lit="O"/>a&jcirc;o (ILO)
        asertas, ke la nova jarmilo komenci&gcirc;is sen solvo por unu
        el la plej gravaj problemoj de la anta&ubreve;a: la sen&ccirc;esa
        kresko de sen<tld/>eco.
        <fnt><aut>E. Georgiev</aut>
        <vrk><url ref="&Monato;/007398.html">Komputika teknologio helpos
        al la senlaboruloj</url></vrk>
        <bib>Monato</bib>
        <lok>2001:05</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_M;&c_ib;&c_zh;&c_n;&c_a;&c_r;&c_o;&c_d;&c_n;&c_a;&c_ja;
    &c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ja;
    <ind>&c_p;&c_r;&c_a;&c_c;&c_y;</ind></trd>,
    <trd>&c_M;&c_A;&c_P;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_m;&c_je;&c_zh;&c_d;&c_u;&c_n;&c_a;&c_r;&c_o;&c_d;&c_n;&c_a; &c_o;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja; &c_n;&c_a; &c_t;&c_r;&c_u;&c_d;&c_a;</trd>,
    <trd>&c_M;&c_O;&c_T;</trd>
  </trdgrp>
  <trd lng="br">Aozadur Etrebroadel al <ind>Labour</ind></trd>
  <trdgrp lng="ca">
    <trd>Organitzaci&oacute; Internacional del Treball</trd>,
    <trd>OIT</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Internationale <ind>Arbeitsorganisation</ind></trd>,
    <trd>IAO</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>International <ind>Labour</ind> Organisation</trd>,
    <trd>ILO</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>Organizaci&oacute;n Internacional del <ind>Trabajo</ind></trd>,
    <trd>OIT</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Organisation internationale du <ind>travail</ind></trd>,
    <trd>OIT</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>Nemzetk&ouml;zi <ind>Munka&uuml;gyi Szervezet</ind></trd>,
    <trd>ILO</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>Internationale <ind>Arbeidsorganisatie</ind></trd>,
    <trd>IAO</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>Mi&eogonek;dzynarodowa Organizacja <ind>Pracy</ind></trd>,
    <trd>MOP</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>Organiza&ccedil;&atilde;o Internacional do
    <ind>Trabalho</ind></trd>,
    <trd>OIT</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>Organiza&#x21B;ia Interna&#x21B;ional&abreve; a <ind>Muncii</ind></trd>,
    <trd>OIM</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_M;&c_je;&c_zh;&c_d;&c_u;&c_n;&c_a;&c_r;&c_o;&c_d;&c_n;&c_a;&c_ja;
    &c_O;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;
    <ind>&c_T;&c_r;&c_u;&c_d;&c_a;</ind></trd>,
    <trd>&c_M;&c_O;&c_T;</trd>
  </trdgrp>
</drv>

</art>
