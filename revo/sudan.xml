<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>Sudan</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="sudan.S0o">
  <kap>
    <ofc>8</ofc><tld lit="S"/>o
    <fnt><bib>RLN</bib></fnt>
  </kap>
  <mlg kod="ISO-3166">SD</mlg>
  <snc mrk="sudan.S0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&Scirc;tato</ref>
      en la nordorienta <ref tip="malprt" cel="afrik.A0o">Afriko</ref> kun &ccirc;efurbo Kartumo;
      Teritorio de Respubliko Sudano,
      limanta kun
      <ref tip="vid" cel="afrik.CentrA0o.GEOG">Centr-Afriko</ref>,
      <ref tip="vid" cel="cxad.CX0o.GEOG">&Ccirc;ado</ref>,
      <ref tip="vid" cel="egipt.E0ujo.GEO">Egiptujo</ref>,
      <ref tip="vid" cel="eritre.E0o.GEOG">Eritreo</ref>,
      <ref tip="vid" cel="etiop.E0ujo.GEOG">Etiopujo</ref>,
      <ref tip="vid" cel="kenj.K0o.GEOG">Kenjo</ref>,
      <ref tip="vid" cel="kong.K0oKinsxasa.GEOG">Kongo
      Kin&scirc;asa</ref>
      <ref tip="vid" cel="libi.L0o.GEOG">Libio</ref>,
      <ref tip="vid" cel="ugand.U0o.GEOG">Ugando</ref>
      kaj <ref tip="vid" cel="mar.RugxaM0o">Ru&gcirc;a <tld lit="M"/>o</ref>:
      <ekz>
         en Zairio kaj <tld/>o, kvaza&ubreve; subite, malsani&gcirc;is pli ol 500 homoj, el kiuj <esc>plejparto</esc> pereis precipe pro hemoragia sindromo
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="https://www.monato.be/2004/008278.html">Damokla glavo super la homaro</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li enga&gcirc;i&gcirc;is por helpi la endan&gcirc;erigitan popolon de <nom>Darfur</nom> en <tld/>o starigi tendaron por rifu&gcirc;intoj
        <fnt>
          <bib>Monato</bib>
          <aut>Zlatko Ti&scaron;ljar</aut>
          <vrk><url ref="&Monato;/2006/008928.php">Prezidi la propran psikon</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    </snc>
    <trd lng="ar">&a_A;&a_l;&a_s;&a_w;&a_d;&a_A;&a_n;</trd>
    <trd lng="be">&c_S;&c_u;&c_d;&c_a;&c_n;</trd>
    <trd lng="bg">&c_S;&c_u;&c_d;&c_a;&c_n;</trd>
    <trdgrp lng="ca">
      <trd>el Sudan</trd>,
      <trd>Rep&uacute;blica del Sudan</trd>
    </trdgrp>
    <trd lng="de">Sudan</trd>
    <trd lng="en">Sudan</trd>
    <trd lng="fa">&a_s;&a_w;&a_d;&a_A;&a_n;</trd>
    <trd lng="fr">Soudan</trd>
    <trd lng="id">Sudan</trd>
    <trd lng="it">Sudan</trd>
    <trd lng="nl">Soedan</trd>
    <trd lng="no">Sudan</trd>
    <trdgrp lng="pl">
      <trd>Sudan</trd>,
      <trd>Republika <ind>Sudanu</ind></trd>
    </trdgrp>
    <trd lng="pt">Sud&atilde;o</trd>
  </drv> 
  

<drv mrk="sudan.0a">
  <kap><tld lit="s"/>a</kap>
  <snc>
    <dif>
    Rilata al la &scirc;tato <tld/>o:
      <ekz>
        li esploris la centran <tld lit="s"/>an montaron
        <fnt>
          <bib>Monato</bib>
          <aut>Zlatko Ti&scaron;ljar</aut>
          <vrk><url ref="&Monato;/2006/009071.php">Pac-peranto liberigita</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="s"/>a islama &scirc;tato da&ubreve;re ilin atakis, celante devigi neislamajn nigrulojn islami&gcirc;i a&ubreve; foriri
        <fnt>
          <bib>Monato</bib>
          <aut>Zlatko Ti&scaron;ljar</aut>
          <vrk><url ref="&Monato;/2006/009071.php">Pac-peranto liberigita</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">sudanesische</trd>
  <trdgrp lng="fr"> 
    <trd>soudanais</trd>
    <trd>soudanaise<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="sudan.0ano">
  <kap><tld lit="s"/>ano,
  <var><kap><tld lit="s"/>anino</kap></var></kap>
  <snc>
    <dif>
    Civitano de <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Sudanese</trd>
  <trdgrp lng="fr"> 
    <trd>Soudanais</trd>
    <trd>Soudanaise<klr> (subst.)</klr></trd>
  </trdgrp>
</drv>

  
</art>
