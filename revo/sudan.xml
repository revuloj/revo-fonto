<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sudan.xml,v 1.21 2020/02/26 22:11:03 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>Sudan</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="sudan.S0o">
  <kap>
    <ofc>8</ofc><tld/>o
    <fnt><bib>RLN</bib></fnt>
  </kap>
  <mlg kod="ISO-3166">SD</mlg>
  <snc mrk="sudan.S0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&Scirc;tato</ref> en la
      nordorienta <ref tip="malprt" cel="afrik.A0o">Afriko</ref> kun
      &ccirc;efurbo Kartumo; Teritorio de Respubliko <tld/>o, limanta kun
      <ref tip="vid" cel="afrik.CentrA0o.GEOG">Centr-Afriko</ref>,
      <ref tip="vid" cel="cxad.CX0o.GEOG">&Ccirc;ado</ref>,
      <ref tip="vid" cel="egipt.E0ujo.GEO">Egiptujo</ref>,
      <ref tip="vid" cel="eritre.E0o.GEOG">Eritreo</ref>,
      <ref tip="vid" cel="etiop.E0ujo.GEOG">Etiopujo</ref>,
      <ref tip="vid" cel="sudan.SudS0o.GEOG">Sud-<tld/>o</ref>,
      <ref tip="vid" cel="ugand.U0o.GEOG">Ugando</ref> kaj
      <ref tip="vid" cel="mar.RugxaM0o">Ru&gcirc;a Maro</ref>:
      <ekz>
         en Zairio kaj <tld/>o, kvaza&ubreve; subite, malsani&gcirc;is pli ol 500
         homoj, el kiuj <esc>plejparto</esc> pereis precipe pro
         hemoragia sindromo
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="https://www.monato.be/2004/008278.html">Damokla
          glavo super la homaro</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li enga&gcirc;i&gcirc;is por helpi la endan&gcirc;erigitan popolon de
        <nom>Darfur</nom> en <tld/>o starigi tendaron por rifu&gcirc;intoj
        <fnt>
          <bib>Monato</bib>
          <aut>Zlatko Ti&scaron;ljar</aut>
          <vrk><url ref="&Monato;/2006/008928.php">Prezidi la propran
          psikon</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ar">&a_A;&a_l;&a_s;&a_w;&a_d;&a_A;&a_n;</trd>
  <trd lng="be">&c_S;&c_u;&c_d;&c_a;&c_n;</trd>
  <trd lng="bg">&c_S;&c_u;&c_d;&c_a;&c_n;</trd>
  <trdgrp lng="ca">
    <trd>el Sudan</trd>,
    <trd>Rep&uacute;blica del Sudan</trd>
  </trdgrp>
  <trd lng="da">Sudan</trd>
  <trd lng="de">Sudan</trd>
  <trd lng="en">Sudan</trd>
  <trd lng="fa">&a_s;&a_w;&a_d;&a_A;&a_n;</trd>
  <trd lng="fr">Soudan</trd>
  <trd lng="id">Sudan</trd>
  <trd lng="it">Sudan</trd>
  <trdgrp lng="nl">
    <trd>Soedan</trd>,
    <trd>Sudan</trd>
  </trdgrp>
  <trd lng="no">Sudan</trd>
  <trdgrp lng="pl">
    <trd>Sudan</trd>,
    <trd>Republika <ind>Sudanu</ind></trd>
  </trdgrp>
  <trd lng="pt">Sud&atilde;o</trd>
  <trd lng="vo">Sud&auml;n</trd>
</drv>

<drv mrk="sudan.0a">
  <kap><tld lit="s"/>a</kap>
  <snc>
    <dif>
      Rilata al la &scirc;tato <tld/>o:
      <ekz>
        li esploris la centran <tld lit="s"/>an montaron
        <fnt>
          <bib>Monato</bib>
          <aut>Zlatko Ti&scaron;ljar</aut>
          <vrk><url ref="&Monato;/2006/009071.php">Pac-peranto
          liberigita</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="s"/>a islama &scirc;tato da&ubreve;re ilin atakis, celante
        devigi neislamajn nigrulojn islami&gcirc;i a&ubreve; foriri
        <fnt>
          <bib>Monato</bib>
          <aut>Zlatko Ti&scaron;ljar</aut>
          <vrk><url ref="&Monato;/2006/009071.php">Pac-peranto
          liberigita</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">sudanesische</trd>
  <trd lng="en">Sudanese</trd>
  <trdgrp lng="fr">
    <trd>soudanais</trd>
    <trd>soudanaise<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>Soedanees</trd>,
    <trd>Sudanees</trd>
  </trdgrp>
  <trd lng="vo">Sud&auml;nik</trd>
</drv>

<drv mrk="sudan.0ano">
  <kap><tld lit="s"/>ano</kap>
  <snc>
    <dif>
      Civitano de <tld/>o:
      <ekz>
        kion ajn ili opinias pri s-ro <nom>Al-Ba&scirc;ir</nom>, la <tld
        lit="s"/>anoj tamen miras pri la nekohereco de Okcidento
        <fnt>
          <aut>J. Tubiana, trad. J.-M. Cash</aut>,
          <vrk><url ref="https://eo.mondediplo.com/article1638.html">
          Pokerludo en Sudano</url></vrk>, 
          <lok>le Monde diplomatique, 2010-07</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Sudanese</trd>
  <trd lng="en">Sudanese</trd>
  <trdgrp lng="fr">
    <trd>Soudanais</trd>
    <trd>Soudanaise<klr> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>Soedanees</trd>,
    <trd>Sudanees</trd>
  </trdgrp>
  <trd lng="vo">Sud&auml;nan</trd>
</drv>

<drv mrk="sudan.SudS0o">
  <kap>
    Sud-<tld/>o
  </kap>
  <mlg kod="ISO-3166">SS</mlg>
  <snc mrk="sudan.SudS0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&Scirc;tato</ref> en la
      nordorienta <ref tip="malprt" cel="afrik.A0o">Afriko</ref> kun
      &ccirc;efurbo &Jcirc;ubo; Teritorio de Respubliko Sud-<tld/>o, limanta kun
      <ref tip="vid" cel="afrik.CentrA0o.GEOG">Centr-Afriko</ref>,
      <ref tip="vid" cel="etiop.E0ujo.GEOG">Etiopujo</ref>,
      <ref tip="vid" cel="kenj.K0o.GEOG">Kenjo</ref>,
      <ref tip="vid" cel="kong.K0oKinsxasa.GEOG">Kongo Kin&scirc;asa</ref>,
      <ref tip="vid" cel="sudan.S0o.GEOG"><tld/>o</ref> kaj
      <ref tip="vid" cel="ugand.U0o.GEOG">Ugando</ref>:
      <ekz>
        &Ccirc;inujo e&ccirc; partoprenis la usonan mediacion inter Kartumo kaj
        &Jcirc;ubo, &ccirc;efurbo de Sud-<tld/>o, kiu heredis la plimulton de
        petrol&scirc;aktoj okaze de la lando-disi&gcirc;o de 2011
        <fnt>
          <aut>A.-C. Robert, trad. J.-M. Cash</aut>,
          <vrk><url ref="https://eo.mondediplo.com/article2448.html">
          Korupto kaj favorigo en Afriko</url></vrk>, 
          <lok>le Monde diplomatique, 2017-01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ar">&a_j;&a_n;&a_w;&a_b; &a_A;&a_l;&a_s;&a_w;&a_d;&a_A;&a_n;</trd>
  <trd lng="be">&c_P;&c_a;&c_w;&c_d;&c_n;&c_jo;&c_v;&c_y; &c_S;&c_u;&c_d;&c_a;&c_n;</trd>
  <trd lng="bg">&c_Ju;&c_zh;&c_je;&c_n; &c_S;&c_u;&c_d;&c_a;&c_n;</trd>
  <trdgrp lng="ca">
    <trd>el Sudan del Sud</trd>,
    <trd>Rep&uacute;blica del Sudan del Sud</trd>
  </trdgrp>
  <trd lng="da">Sydsudan</trd>
  <trd lng="de">S&uuml;dsudan</trd>
  <trd lng="en">South Sudan</trd>
  <trd lng="fa">&a_s;&a_w;&a_d;&a_A;&a_n; &a_j;&a_n;&a_w;&a_b;&f_y;</trd>
  <trd lng="fr">Soudan du Sud</trd>
  <trd lng="id">Sudan Selatan</trd>
  <trd lng="it">Sudan del Sud</trd>
  <trdgrp lng="nl">
    <trd>Zuid-Soedan</trd>,
    <trd>Zuid-Sudan</trd>
  </trdgrp>
  <trd lng="no">S&oslash;r-Sudan</trd>
  <trdgrp lng="pl">
    <trd>Sudan Po&lstroke;udniowy</trd>,
    <trd>Republika <ind>Sudanu Po&lstroke;udniowego</ind></trd>
  </trdgrp>
  <trd lng="pt">Sud&atilde;o do Sul</trd>
  <trd lng="vo">Sul&uuml;da-Sud&auml;n</trd>
</drv>

<drv mrk="sudan.sud0a">
  <kap>sud-<tld lit="s"/>a</kap>
  <snc>
    <dif>
      Rilata al la &scirc;tato Sud-<tld/>o:
      <ekz>
        &ccirc;i tiu decido estis respondo al la konfiskado fare de &Hcirc;artumo de
        parto de la sud-<tld lit="s"/>a produkto
        <fnt>
          <aut>J.-B. Gallopin, trad. J.-M. Cash</aut>,
          <vrk><url ref="https://eo.mondediplo.com/article1849.html">
          Amara divorco de amba&ubreve; Sudanoj</url></vrk>, 
          <lok>le Monde diplomatique, 2012-06</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">s&uuml;dsudanesische</trd>
  <trd lng="en">South Sudanese</trd>
  <trdgrp lng="nl">
    <trd>Zuid-Soedanees</trd>,
    <trd>Zuid-Sudanees</trd>
  </trdgrp>
  <trd lng="vo">Sul&uuml;da-Sud&auml;nik</trd>
</drv>

<drv mrk="sudan.sud0ano">
  <kap>sud-<tld lit="s"/>ano</kap>
  <snc>
    <dif>
      Civitano de Sud-<tld/>o:
      <ekz>
        en junio 2016 okazis en &Jcirc;ubo, la &ccirc;efurbo, granda batalado, en
        kiu mortis pli ol 270 sud-<tld lit="s"/>anoj kaj du &ccirc;inaj
        soldatoj de la pactrupo de UN
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url
          ref="https://www.monato.be/2018/012206.php">Demokratio kaj
          informado</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">S&uuml;dsudanese</trd>
  <trd lng="en">South Sudanese</trd>
  <trdgrp lng="nl">
    <trd>Zuid-Soedanees</trd>,
    <trd>Zuid-Sudanees</trd>
  </trdgrp>
  <trd lng="vo">Sul&uuml;da-Sud&auml;nan</trd>
</drv>

</art>
