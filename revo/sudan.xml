<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>sudan</rad>/o
  <ofc>8</ofc>
</kap>

<drv mrk="sudan.0o">
  <kap>
    <tld lit="S"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="sudan.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la nordorienta <ref tip="malprt"
      cel="afrik.A0o">Afriko</ref> kun &ccirc;efurbo Kartumo.
      Teritorio de Respubliko Sudano.
      ISO-3166 kodo: SD.
      &Gcirc;i limas kun
      <ref tip="vid" cel="afrik.centr0o.GEOG">Centr-Afriko</ref>,
      <ref tip="vid" cel="cxad.0o.GEOG">&Ccirc;ado</ref>,
      <ref tip="vid" cel="egipt.E0ujo.GEO">Egiptujo</ref>,
      <ref tip="vid" cel="eritre.0o.GEOG">Eritreo</ref>,
      <ref tip="vid" cel="etiop.0ujo.GEOG">Etiopujo</ref>,
      <ref tip="vid" cel="kenj.0o.GEOG">Kenjo</ref>,
      <ref tip="vid" cel="kong.0o.kinsxasa.GEOG">Kongo
      Kin&scirc;asa</ref>
      <ref tip="vid" cel="libi.0o.GEOG">Libio</ref>,
      <ref tip="vid" cel="ugand.0o.GEOG">Ugando</ref>
      kaj <ref tip="vid" cel="mar.RugxaM0o">Ru&gcirc;a <tld
      lit="M"/>o</ref>.
    </dif>
    <trd lng="ar">&a_A;&a_l;&a_s;&a_w;&a_d;&a_A;&a_n;</trd>
    <trd lng="bg">&c_S;&c_u;&c_d;&c_a;&c_n;</trd>
    <trd lng="de">Sudan</trd>
    <trd lng="en">Sudan</trd>
    <trd lng="it">Sudan</trd>
    <trdgrp lng="pl">
      <trd>Sudan</trd>,
      <trd>Republika <ind>Sudanu</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

