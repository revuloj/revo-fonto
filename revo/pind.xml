<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pind.xml,v 1.22 2023/09/03 12:45:07 revo Exp $">
<kap>
  <rad>pind</rad>/o
  <fnt><bib>PIV1</bib></fnt>,
  <var><kap><rad var="aj">pajnt</rad>/o</kap></var>
</kap>
<drv mrk="pind.0o">
  <kap><tld/>o,
  <var><kap><tld var="aj"/>o</kap></var></kap>
  <snc mrk="pind.0o.MEZ">
    <dif>
      Tradicia <ref tip="lst" cel="unu.mezur0o" lst="voko:kvantoj">mezurunuo</ref> de
      volumeno por trinka&jcirc;oj, diversvalora la&ubreve; landoj:
      <ekz>
        li trinkis pli ol unu <tld/>on el la
        kvanto
        <fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 17a, p.
        119a</lok></fnt>.
      </ekz>
    </dif>
    <subsnc mrk="pind.0o.brita">
      <dif>
        brita <tld/>o: 0,568 da litroj:
        <ekz>
          unu brita <tld/>o estas duono de <ref tip="malprt" cel="kvart1.0o">kvarto</ref>
          kaj okono de <ref tip="malprt" cel="galjon.0o">galjono</ref>;
        </ekz>
        <ekz>
          lando de ekstravagancaj insulanoj kun mal&gcirc;ustaflanka trafiko, dueta&gcirc;aj
          omnibusoj, mejloj, <tld var="aj"/>oj, futoj, pori&gcirc;o, elo
          <fnt>
            <bib>LOdE</bib>
            <lok>2003, &#x2116; 6 (104)</lok>
          </fnt>;
        </ekz>
        <ekz>
          li proksimi&gcirc;is al la ver&scirc;ejo, kaj replenigis <klr>[sian vezikon]</klr> per
          kelkaj <tld var="aj"/>oj da bona irlanda portero
          <fnt>
            <bib>Monato</bib>
            <aut>Albisturo Kvinke</aut>
            <vrk><url ref="&Monato;/2014/010918.php">Mirakloj</url></vrk>
            <lok>2014</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="pind.0o.hungara">
      <dif>
        hungara kaj viena <tld/>oj: 1,5 da litroj.
      </dif>
    </subsnc>
    <subsnc mrk="pind.0o.kanada">
      <dif>
        kanada <tld/>o: 1,136 da litroj.
      </dif>
    </subsnc>
  </snc>
  <refgrp tip="vid">
    <ref cel="stof.0o">stofo</ref>
  </refgrp>
  <trd lng="be">&c_p;&c_ib;&c_n;&c_t;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>m&aacute;z</trd>,
    <trd>pinta</trd>
  </trdgrp>
  <trd lng="de">Pinte</trd>
  <trd lng="en">pint</trd>
  <trd lng="es">pinta<klr> (unidad de medida)</klr></trd>
  <trd lng="fr">pinte<klr> (mesure)</klr></trd>
  <trd lng="hu">pint</trd>
  <trd lng="ja">&#x30D1;&#x30A4;&#x30F3;&#x30C8;</trd>
  <trd lng="nl">pint</trd>
  <trd lng="pl">pinta</trd>
  <trd lng="pt">pint</trd>
  <trd lng="ro">pint</trd>
  <trd lng="ru">&c_p;&c_i;&c_n;&c_t;&c_a;</trd>
  <trd lng="sk">pinta <klr>(jednotka dutej miery)</klr></trd>

  <trd lng="uk">&#x43F;&c_ib;&#x43D;&c_t;&c_a; (&#x43C;&c_ib;&c_r;&c_a; &c_jeu;&#x43C;&#x43D;&#x43E;&c_s;&c_t;&c_ib;)</trd>
</drv>
</art>

