<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pind.xml,v 1.16 2022/03/23 21:23:38 revo Exp $">
<kap>
  <rad>pind</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="pind.0o">
  <kap><tld/>o</kap>
  <snc mrk="pind.0o.MEZ">
    <dif>
      Tradicia <ref tip="lst" cel="unu.mezur0o"
      lst="voko:kvantoj">mezurunuo</ref> de
      kapacito por trinka&jcirc;oj, diversvalora la&ubreve; landoj:
      <ekz>
        li trinkis pli ol unu <tld/>on el la
        kvanto
        <fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 17a, p.
        119a</lok></fnt>.
      </ekz>
    </dif>
    <subsnc>
      <dif>
        brita <tld/>o: 0,568 da litroj:
        <ekz>
          unu brita <tld/>o estas duono de <ref tip="malprt" cel="kvart1.0o">kvarto</ref> 
          kaj okono de <ref tip="malprt" cel="galjon.0o">galjono</ref>;
        </ekz>
      </dif>
    </subsnc>
    <subsnc>
      <dif>
        hungara kaj Viena <tld/>oj: 1,5 da litroj.
      </dif>
    </subsnc>
    <subsnc>
      <dif>
        kanada <tld/>o: 1,136 da litroj.
      </dif>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="stof.0o">stofo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_n;&c_t;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>m&aacute;z</trd>,
    <trd>pinta</trd>
  </trdgrp>
  <trd lng="de">Pinte</trd>
  <trd lng="en">pint</trd>
  <trd lng="es">pinta<klr> (unidad de medida)</klr></trd>
  <trd lng="fr">pinte<klr> (mesure)</klr></trd>
  <trd lng="hu">pint</trd>
  <trd lng="nl">pint</trd>
  <trd lng="pl">pinta</trd>
  <trd lng="pt">pint</trd>
  <trd lng="ro">pint</trd>
  <trd lng="ru">&c_p;&c_i;&c_n;&c_t;&c_a;</trd>
  <trd lng="sk">pinta <klr>(jednotka dutej miery)</klr></trd>

</drv>
</art>

