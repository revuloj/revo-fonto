<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>pind</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="pind.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Tradicia <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de
      kapacito por trinka&jcirc;oj, diversvalora la&ubreve; landoj:
      <ekz>
        li trinkis pli ol unu <tld/>on el la
        kvanto<fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 17a, p.
        119a</lok></fnt>.
      </ekz>
    </dif>
    <subsnc>
      <dif>
        brita <tld/>o: 0,568 litro.
      </dif>
    </subsnc>
    <subsnc>
      <dif>
        kanada <tld/>o: 1,136 litro.
      </dif>
    </subsnc>
  </snc>
  <trd lng="fr">pinte<klr> (mesure)</klr></trd>
</drv>
</art>

