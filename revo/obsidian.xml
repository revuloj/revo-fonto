<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: obsidian.xml,v 1.12 2024/01/30 16:09:13 revo Exp $">
<kap>
  <rad>obsidian</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="obsidian.0o">
  <kap><tld/>o</kap>
  <snc mrk="obsidian.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Vulkandevena <ref tip="super" cel="sxton.0a.MIN">&scirc;ton</ref>speco 
      vitreca, tre malmola:
      <ekz>
        jam dum prahistorio obsidiano estis uzita por krei &scirc;tonajn ilojn
        <fnt>
          <vrk><url ref="&Viki;/Obsidiano">Obsidiano</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        metaloj mankis en Pacifiko, sed de loko al loko trovi&gcirc;is la 
        vulkana vitro, <tld/>o, el kiu ili splitis tran&ccirc;ilojn
        <fnt>
          <aut>B. Fox</aut>,
          <vrk><url ref="http://ourworld.compuserve.com/homepages/profcon/a_fox.htm">
          La Maorioj de Nov-Zelando</url></vrk>,
          <lok>[1997?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_s;&c_y;&c_d;&c_y;&c_ja;&c_n;</trd>
  <trd lng="bg">&c_o;&c_b;&c_s;&c_i;&c_d;&c_i;&c_a;&c_n;</trd>
  <trd lng="cs">obsidi&aacute;n</trd>
  <trd lng="de">Obsidian</trd>
  <trd lng="en">obsidian</trd>
  <trd lng="fr">obsidienne</trd>
  <trd lng="it">ossidiana</trd>
  <trd lng="nl">obsidiaan</trd>
  <trd lng="pl">obsydian</trd>
  <trd lng="sk">obsidi&aacute;n</trd>
  <trd lng="sv">obsidian</trd>
  <trdgrp lng="uk">
    <trd>&#x43E;&c_b;&c_s;&c_i;&c_d;&c_ib;&c_a;&#x43D;</trd>,
    <trd>&c_v;&c_u;&#x43B;&#x43A;&c_a;&#x43D;&c_ib;&c_ch;&#x43D;&c_je; &c_s;&#x43A;&#x43B;&#x43E;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9ED1;&#x66DC;&#x5CA9; <pr>h&#x113;iy&#xE0;oy&#xE1;n</pr></trd>,
    <trd>&#x9ED1;&#x66DC;&#x77F3; <pr>h&#x113;iy&#xE0;osh&#xED;</pr></trd>
  </trdgrp>
</drv>
</art>

