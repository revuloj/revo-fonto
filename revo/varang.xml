<kap>
  <rad>varang</rad>/o
</kap>
<drv mrk="varang.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Malsupra parto de la <ref tip="malprt" cel="sxip.0o">&scirc;ipaj</ref>
      ripoj, kunigitaj kun la <ref tip="malprt" cel="kil.0o">kilo</ref>:
      <ekz>
        varango estas esenca konstruparto por plifortigi la trunkon (hulon)
        de boatoj, &scirc;ipoj, aviadiloj kaj aliaj veturiloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=105052">Varango</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_l;&c_o;&c_r;</trd>
  <trd lng="ca">varenga</trd>
  <trdgrp lng="cs">
    <trd>b&rcaron;ich lodi</trd>,
    <trd>trup lodi</trd>
  </trdgrp>
  <trd lng="de">Bodenwrange</trd>
  <trd lng="en">floor plate, floor timber<klr> (dividing outer bottom
    from inner)</klr></trd>
  <trd lng="es">varenga</trd>
  <trd lng="fr">varangue</trd>
  <trdgrp lng="hu">
    <trd>b&oacute;konytalp</trd>,
    <trd>bordatalp</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x808B;&#x677F; <pr>&#12429;&#12367;&#12400;&#12435;</pr></trd>,
    <trd>&#33337;&#x5E95;&#x5E8A;&#x677F;  <pr>&#x305B;&#12435;&#12390;&#12356;&#12422;&#x304B;&#12356;&#x305F;</pr></trd>,
    <trd>&#x808B;&#26681;&#26448;  <pr>&#12429;&#12387;&#12371;&#12435;&#12374;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">wrang<klr tip="ind"> (v.schip)</klr></trd>
  <trd lng="pl">wr&eogonek;ga</trd>
  <trd lng="pt">varengas</trd>
  <trd lng="ru">&c_f;&c_l;&c_o;&c_r;</trd>
  <trd lng="sk">doln&aacute; &ccaron;as&tcaron; lodn&eacute;ho rebrovia</trd>
  <trd lng="uk">&c_sh;&#x43F;&c_a;&#x43D;&c_g;&#x43E;&c_u;&c_t;</trd>
</drv>
