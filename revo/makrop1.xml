<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: makrop1.xml,v 1.19 2023/10/19 15:13:16 revo Exp $">
<kap>
  <rad>makroped</rad>/oj <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="makrop1.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="makrop1.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la subklaso
      <ref tip="malprt" cel="marsup.0uloj.ZOO">marsupiuloj</ref>
      (<trd lng="la">Macropodidae</trd>):
      <ekz>
        <klr>[al]</klr> la <tld/>oj <klr>[&#x2026;]</klr> apartenas kanguruoj,
        valabioj,
        kvokaoj, arbokanguruoj <klr>[&#x2026;]</klr> kaj pluraj aliaj
        <klr>[ili]</klr>
        hejmas en la a&ubreve;stralia kontinento
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=584367">Makropedoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="kangur.0o">kanguruo</ref>
  </snc>
  <trd lng="be">&c_k;&c_je;&c_n;&c_g;&c_u;&c_r;&c_o;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">macrop&ograve;dids</trd>
  <trd lng="cs">klokanovit&iacute;</trd>
  <trd lng="de">K&auml;ngurus<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="en">kangaroos</trd>
  <trdgrp lng="fr">
    <trd>kangourous</trd>,
    <trd>macropodid&eacute;s</trd>
  </trdgrp>
  <trd lng="hu">kenguruf&eacute;l&eacute;k</trd>
  <trd lng="ja">&#x30AB;&#x30F3;&#x30AC;&#x30EB;&#x30FC;&#x79D1;  <pr>&#x30AB;&#x30F3;&#x30AC;&#x30EB;&#x30FC;&#x304B;</pr></trd>
  <trd lng="nl">kangoeroes<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pl">kangurowate</trd>
  <trd lng="pt">macropod&iacute;deos</trd>
  <trd lng="ro">canguri</trd>
  <trd lng="ru">&c_k;&c_je;&c_n;&c_g;&c_u;&c_r;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">klokanovit&iacute;</trd>
  <trd lng="uk">&#x43A;&#x435;&#x43D;&#x433;&#x443;&#x440;&#x43E;&#x432;&#x456;</trd>
</drv>
</art>
