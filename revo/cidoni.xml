<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>cidoni</rad>/o
</kap>
<drv mrk="cidoni.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Frukto de <tld/>ujo, uzata ekz. por konfita&jcirc;oj.
    </dif>
    <ref tip="super" cel="frukt.plur0o">plurfrukto</ref>
  </snc>
  <trd lng="de">Quitte <klr tip="ind">(Frucht)</klr></trd>
  <trd lng="fr">coing</trd>
  <trd lng="hu">birsalma</trd>
  <trd lng="ru">&c_a;&c_j;&c_v;&c_a; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
</drv>
<drv mrk="cidoni.0arbo0ujo">
  <kap><tld/>arbo, <tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="rozac.0oj">rozacoj</ref>
      (<trd lng="la">Cydonia</trd>), kun pir- a&ubreve; pomsimilaj
      flavaj lanug&scirc;elaj fruktoj.
    </dif>
  </snc>
  <trd lng="de">Quitte <klr tip="ind">(Baum)</klr></trd>
  <trd lng="fr">cognassier</trd>
  <trd lng="hu">birsalmafa</trd>
  <trd lng="ru">&c_a;&c_j;&c_v;&c_a;
  <klr>(&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;)</klr></trd>
</drv>
</art>
