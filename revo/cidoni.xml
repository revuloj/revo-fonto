<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>cidoni</rad>/o
</kap>
<drv mrk="cidoni.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Frukto de <tld/>ujo. <tld lit="C"/>oj estas uzataj
      ekz. por konfita&jcirc;oj.
    </dif>
  </snc>
  <trd lng="de">Quitte <klr tip="ind">(Frucht)</klr></trd>
</drv>
<drv mrk="cidoni.0arbo0ujo">
  <kap><tld/>arbo, <tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="roz.0acoj">rozacoj</ref>
      (<trd lng="la">Cydonia</trd>), kun pir- a&ubreve; pomsimilaj
      flavaj lanug&scirc;elaj fruktoj.
    </dif>
  </snc>
  <trd lng="de">Quitte <klr tip="ind">(Baum)</klr></trd>
</drv>
</art>
