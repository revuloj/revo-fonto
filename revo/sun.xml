<drv mrk="sun.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sun.0o.nia">
  <mlg>&#9737;</mlg>
    <dif>
      La <ref tip="super" cel="stel.0o.AST">stelo</ref>,
      &ccirc;irka&ubreve;
      kiu movi&gcirc;as la tero:
      <ekz>
        mi estas la plej feli&ccirc;a homo sub la <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        post vetero malbela lumas <tld/>o plej hela
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="ca">Sol</trd>
    <trd lng="de">Sonne</trd>
    <trdgrp lng="en">
      <trd><klr>the </klr><ind>Sun</ind></trd>,
      <trd>Sol<klr> (the Sun)</klr></trd>
    </trdgrp>
    <trd lng="he">&het;&mem;&he;</trd>
    <trd lng="hu">Nap</trd>
  </snc>
  <snc mrk="sun.0o.stelo">
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="dif" cel="stel.0o.AST">stelo</ref>
      <ekz>
        &gcirc;ia meza distanco de &gcirc;ia <tld/>o estas 0,3 astronomiaj unuoj (= Suno-Tero-distanco)
        <fnt>
          <bib>Monato</bib>
          <vrk>Du strangaj planedsistemoj</vrk>
          <lok>jaro 2001a, numero 7a, p. 20a</lok>
        </fnt>;
      </ekz>
      <ekz>
        planedoj, kiuj &ccirc;irka&ubreve;as <tld/>ojn for de nia sunsistemo
        <fnt>
          <bib>LOdE</bib>
          <aut>Amri Wandel</aut>
          <vrk><url ref="https://sezonoj.ru/2017/06/wandel/"
              >Astrobiologio kaj la perspektivo de vivo ekstertera</url></vrk>
          <lok>20 junio 2017</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Zentralgestirn</trd>
    <trd lng="hu">csillag</trd>
    <trdgrp lng="ca">
      <trd>estel</trd>,
      <trd>astre</trd>
    </trdgrp>
    <trd lng="en">star<klr> (n) (sun)</klr></trd>
  </snc>
  <snc mrk="sun.0o.radioj">
    <dif>
      <tld lit="S"/>aj radioj trafantaj la supra&jcirc;on de nia
      terglobo:
      <ekz>
        ni &gcirc;uis agrablan <tld/>on.
      </ekz>
    </dif>
    <trd lng="ca">llum solar</trd>
    <trd lng="de">Sonnenstrahlen</trd>
    <trd lng="en">sun<klr> (n) (weather)</klr></trd>
    <trd lng="es">rayos solares</trd>
    <trd lng="hu">napf&eacute;ny</trd>
  </snc>
  <!-- SUPERFLUA DIFINO
  <snc mrk="sun.0o.figuro">
    <dif>
      <ref tip="super" cel="figur.0o">Figuro</ref> simbolanta la
      <tld/>on.
    </dif>
  </snc>
  -->
  <trd lng="af">son</trd>
  <trd lng="am">&#4928;&#4624;&#4845;</trd>
  <trd lng="ar">&a_sh;&a_m;&a_s;</trd>
  <trd lng="be">&c_s;&c_o;&c_n;&c_c;&c_a;</trd>
  <trd lng="bg">&c_s;&c_l;&c_malmol;&c_n;&c_c;&c_je;</trd>
  <trd lng="bn">&#2488;&#2498;&#2480;&#2509;&#2479;</trd>
