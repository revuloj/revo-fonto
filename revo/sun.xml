<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sun.xml,v 1.47 2021/05/28 17:04:38 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sun</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="sun.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sun.0o.nia">
  <mlg>&#x2609;</mlg>
    <dif>
      La <ref tip="super" cel="stel.0o.AST">stelo</ref>,
      &ccirc;irka&ubreve;
      kiu movi&gcirc;as la tero:
      <ekz>
        mi estas la plej feli&ccirc;a homo sub la <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        post vetero malbela lumas <tld/>o plej hela
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="ca">Sol</trd>
    <trd lng="de">Sonne</trd>
    <trd lng="he">&het;&mem;&he;</trd>
    <trd lng="hu">Nap</trd>
  </snc>
  <snc mrk="sun.0o.stelo">
    <uzo tip="fak">AST</uzo>
    <ref tip="dif" cel="stel.0o.AST">stelo</ref>
    <trd lng="de">Zentralgestirn</trd>
    <trd lng="hu">csillag</trd>
    <trdgrp lng="ca">
      <trd>estel</trd>,
      <trd>astre</trd>
    </trdgrp>
  </snc>
  <snc mrk="sun.0o.radioj">
    <dif>
      <tld lit="S"/>aj radioj trafantaj la supra&jcirc;on de nia
      terglobo:
      <ekz>
        ni &gcirc;uis agrablan <tld/>on.
      </ekz>
    </dif>
    <trd lng="ca">llum solar</trd>
    <trd lng="de">Sonnenstrahlen</trd>
    <trd lng="es">rayos solares</trd>
    <trd lng="hu">napf&eacute;ny</trd>
  </snc>
  <!-- SUPERFLUA DIFINO
  <snc mrk="sun.0o.figuro">
    <dif>
      <ref tip="super" cel="figur.0o">Figuro</ref> simbolanta la
      <tld/>on.
    </dif>
  </snc>
  -->
  <trd lng="af">son</trd>
  <trd lng="be">&c_s;&c_o;&c_n;&c_c;&c_a;</trd>
  <trd lng="bg">&c_s;&c_l;&c_malmol;&c_n;&c_c;&c_je;</trd>
  <trd lng="ca">sol</trd>
  <trd lng="cs">slunce</trd>
  <trd lng="de">Sonne</trd>
  <trd lng="en">sun</trd>
  <trd lng="es">sol</trd>
  <trd lng="fr">soleil</trd>
  <trd lng="fy">sinne</trd>
  <trd lng="he">&shin;&mem;&shin;</trd>
  <trd lng="ia">sol</trd>
  <trd lng="id">matahari</trd>
  <trd lng="io">suno</trd>
  <trd lng="it">sole</trd>
  <trdgrp lng="ja">
    <trd>&#x592A;&#x967D;
    [<ind>&#x305F;&#x3044;&#x3088;&#x3046;</ind>]</trd>,
    <trd>&#x65E5; [<ind>&#x3072;</ind>]</trd>
  </trdgrp>
  <trd lng="jbo">solri</trd>
  <trd lng="ne">&#x938;&#x942;&#x930;&#x94D;&#x92F;</trd>
  <trd lng="nl">zon</trd>
  <trd lng="pl">s&lstroke;o&nacute;ce</trd>
  <trd lng="pt">sol</trd>
  <trd lng="ro">soare</trd>
  <trd lng="ru">&c_s;&c_o;&c_l;&c_n;&c_c;&c_je;</trd>
  <trd lng="sv">sol</trd>
  <trd lng="tp">suno</trd>
  <trd lng="tr">g&uuml;ne&scedil;</trd>
  <trd lng="vo">sol</trd>
  <trdgrp lng="zh">
    <trd>&#x65E5;<klr> [r&igrave;]</klr></trd>,
    <trd>&#x592A;&#x9633;</trd>,
    <trd><klr>tr. </klr>&#x592A;&#x967D;<klr>
    [t&agrave;iy&aacute;ng]</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>slnce</trd>,
    <trd>slnko</trd>
  </trdgrp>
</drv>

<drv mrk="sun.0umi">
  <kap><tld/>umi</kap>
  <snc mrk="sun.0umi.KOMUNE">
    <dif>
      Elmeti al <tld/>lumo por varmigi, sekigi...:
      <ekz>
        &Uacute;rsula memoris la solan aferon nekutiman, kiun Fernanda
        faris tiutage: <tld/>umi la matojn de la infanoj
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de
        Diego</aut>,
        <vrk>Cent
        Jaroj da Soleco</vrk>, <lok>1992</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>posar al sol</trd>,
    <trd>solejar</trd>,
    <trd>assolellar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>oh&rcaron;&iacute;vat na slunci</trd>,
    <trd>opalovat</trd>,
    <trd>slunit se</trd>
  </trdgrp>
  <trd lng="fr">exposer au soleil</trd>
  <trd lng="it">esporre al <ind>sole</ind></trd>
  <trdgrp lng="sk">
    <trd>opa&lcaron;ova&tcaron; sa</trd>,
    <trd>slni&tcaron; sa</trd>
  </trdgrp>
</drv>

<drv mrk="sun.0umigxi">
  <kap><tld/>umi&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sun.0umigxi.KOMUNE">
    <dif>
      Elmeti&gcirc;i al <tld/>radiado:
      <ekz>
        onklo Pan kondukis la junan bovinon sur la vila&gcirc;an 
        placon por <tld/>umi&gcirc;i
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>l&eacute;zarder<klr> (au soleil)</klr></trd>,
    <trd>prendre un bain de <ind>soleil</ind></trd>
  </trdgrp>
</drv>

<drv mrk="sun.0sistemo">
  <kap><tld/>sistemo</kap>
  <snc mrk="sun.0sistemo.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="prt" cel="sun.0o.stelo"><tld lit="S"/>o</ref> kune kun
      la &gcirc;in &ccirc;irka&ubreve;antaj astroj.
    </dif>
  </snc>
  <ref tip="malprt" cel="galaksi.0o">galaksio</ref>
  <trd lng="be">&c_s;&c_o;&c_n;&c_je;&c_ch;&c_n;&c_a;&c_ja; &c_s;&c_y;&c_s;&c_t;&c_e;&c_m;&c_a;</trd>
  <trd lng="bo">&#xF49;&#xF72;&#xF0B;&#xF58;&#xF60;&#xF72;&#xF0B;&#xF41;&#xFB1;&#xF72;&#xF58;&#xF0B;&#xF62;&#xF92;&#xFB1;&#xF74;&#xF51;</trd>
  <trd lng="ca">sistema <ind>sol</ind>ar</trd>
  <trd lng="cs">slune&ccaron;n&iacute; soustava</trd>
  <trd lng="de">Sonnensystem</trd>
  <trd lng="en">solar system</trd>
  <trd lng="es">sistema solar</trd>
  <trd lng="he">&mem;&ayin;&resh;&kaf;&tav; &shin;&mem;&shin;</trd>
  <trd lng="hu">naprendszer</trd>
  <trd lng="fr">syst&egrave;me <ind>solaire</ind></trd>
  <trd lng="fy">sinnestelsel</trd>
  <trd lng="id">tata surya</trd>
  <trd lng="io">sunala sistemo</trd>
  <trd lng="it">sistema <ind>solare</ind></trd>
  <trd lng="nl">zonnestelsel</trd>
  <trd lng="pl">uk&lstroke;ad <ind>s&lstroke;oneczny</ind></trd>
  <trd lng="pt">sistema solar</trd>
  <trd lng="ro">sistemul solar</trd>
  <trd lng="ru">&c_s;&c_o;&c_l;&c_n;&c_je;&c_ch;&c_n;&c_a;&c_ja; &c_s;&c_i;&c_s;&c_t;&c_je;&c_m;&c_a;</trd>
  <trd lng="sk">slne&ccaron;n&aacute; s&uacute;stava</trd>
</drv>

<drv mrk="sun.ekster0sistema">
  <kap>ekster<tld/>sistema</kap>
  <snc mrk="sun.ekster0sistema.KOMUNE">
    <uzo tip="fak">AST</uzo>
    <dif>
      Situanta ekster <tld/>sistemo:
      <ekz>
        <ind>ekster<tld/>sistema planedo</ind>.
        <trdgrp lng="ca">
          <trd>planeta extrasolar</trd>,
          <trd>exoplaneta</trd>
        </trdgrp>
        <trd lng="en">exoplanet</trd>
        <trdgrp lng="it">
          <trd><ind>pianeta</ind> extrasolare</trd>,
          <trd>esopianeta</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">extrasolar</trd>
  <trd lng="it">extrasolare</trd>
</drv>

<drv mrk="sun.0subiro">
  <kap><tld/>subiro</kap>
  <snc mrk="sun.0subiro.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      <ref tip="malprt" cel="vesper.0o">Vespera</ref> malapero de la
      suno:
      <ekz>
        Kiam okazos la <tld/>subiro vendredon?
        <fnt>
          <vrk>Esperanto course for English speakers at Duolingo</vrk>
          <url ref="https://www.duolingo.com/dictionary/Esperanto/sunsubiro/b8fa55112d4888ab983aabad2be4fb03">sunsubiro</url>
        </fnt>
      </ekz>
    </dif>
    <trd lng="cs">z&aacute;pad slunce</trd>
    <trd lng="en">sunset</trd>
    <trd lng="pl">zach&oacute;d s&lstroke;o&nacute;ca</trd>
    <trd lng="sk">z&aacute;pad slnka</trd>
  </snc>
</drv>

</art>
