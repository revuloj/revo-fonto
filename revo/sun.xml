<drv mrk="sun.ekster0sistema">
  <kap>ekster<tld/>sistema</kap>
  <snc mrk="sun.ekster0sistema.KOMUNE">
    <uzo tip="fak">AST</uzo>
    <dif>
      Situanta ekster <tld/>sistemo:
      <ekz>
        la unua <ind>ekster<tld/>sistema planedo</ind> estis eltrovita jam <klr>[&#8230;]</klr> en 1995
        <fnt>
          <bib>Monato</bib>
          <aut>Oliver Kim</aut>
          <vrk><url ref="&Monato;/2006/008796.php">Unua fotografa&jcirc;o de ekstersunsistema
          planedo</url></vrk>
          <lok>2006</lok>
        </fnt>.
        <trd lng="be">&c_e;&c_k;&c_z;&c_a;&c_p;&c_l;&c_ja;&c_n;&c_e;&c_t;&c_a;</trd>
        <trdgrp lng="ca">
          <trd>planeta extrasolar</trd>,
          <trd>exoplaneta</trd>
        </trdgrp>
        <trd lng="en">exoplanet</trd>
        <trdgrp lng="id">
          <trd>eksoplanet</trd>,
          <trd><ind>planet</ind> luar surya</trd>
        </trdgrp>
        <trdgrp lng="it">
          <trd><ind>pianeta</ind> extrasolare</trd>,
          <trd>esopianeta</trd>
        </trdgrp>
        <trdgrp lng="pl">
          <trd>egzoplaneta</trd>,
          <trd>planeta pozas&lstroke;oneczna</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
