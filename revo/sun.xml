<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>sun</rad>/o</kap>
<drv mrk="sun.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="sun.0o.nia">
     <dif>
       La <ref tip="super" cel="stel.0o.AST">stelo</ref>,
&ccirc;irka&ubreve;
       kiu movi&gcirc;as la tero:
       <ekz>
         mi estas la plej feli&ccirc;a homo sub la <tld/>o<fnt>Z</fnt>;
       </ekz>
       <ekz>
         post vetero malbela lumas <tld/>o plej hela
         <fnt><bib>PrV</bib></fnt>.
       </ekz>
     </dif>
     <trd lng="de">Sonne</trd>
     <trd lng="he">&het;&mem;&he;</trd>
     <trd lng="hu">Nap</trd>
   </snc>
   <snc mrk="sun.0o.stelo">
     <uzo tip="fak">AST</uzo>
     <ref tip="dif" cel="stel.0o.AST">stelo</ref>
     <trd lng="de">Zentralgestirn</trd>
     <trd lng="hu">csillag</trd>
   </snc>
   <snc mrk="sun.0o.radioj">
     <dif>
       <tld lit="S"/>aj radioj trafantaj la supra&jcirc;on de nia terglobo:
       <ekz>
         ni &gcirc;uis agrablan <tld/>on.
       </ekz>
     </dif>
     <trd lng="de">Sonnenstrahlen</trd>
     <trd lng="es">rayos solares</trd>
     <trd lng="hu">napf&eacute;ny</trd>
   </snc>
   <!-- SUPERFLUA DIFINO
   <snc mrk="sun.0o.figuro">
     <dif>
       <ref tip="super" cel="figur.0o">Figuro</ref> simbolanta la <tld/>on.
     </dif>
   </snc>
   -->
   <trd lng="be">&c_s;&c_o;&c_n;&c_c;&c_a;</trd>
   <trd lng="cs">slunce</trd>
   <trd lng="de">Sonne</trd>
   <trd lng="en">sun</trd>
   <trd lng="es">sol</trd>
   <trd lng="fr">soleil</trd>
   <trd lng="he">&shin;&mem;&shin;</trd>
   <trd lng="it">sole</trd>
   <trd lng="nl">zon</trd>
   <trd lng="pl">s&lstroke;o&nacute;ce</trd>
   <trd lng="ro">soare</trd>
   <trd lng="ru">&c_s;&c_o;&c_l;&c_n;&c_c;&c_je;</trd>
   <trd lng="sv">sol</trd>
   <trd lng="tr">g&uuml;ne&scedil;</trd>
   <trd lng="vo">sol</trd>
   <trdgrp lng="zh">
     <trd>&#26085;<klr> [r&igrave;]</klr></trd>,
     <trd>&#22826;&#38451;</trd>,
     <trd><klr>tr. </klr>&#22826;&#38525;<klr>
[t&agrave;iy&aacute;ng]</klr></trd>
   </trdgrp>
</drv>

<drv mrk="sun.0floro">
   <kap><tld/>floro</kap>
   <snc>
     <ref tip="dif" cel="helian.0o">Helianto</ref>
     <bld lok="../bld/heliant.jpg">
       <tld/>floroj <klr>(Helianthus annuus)</klr>
     </bld>
   </snc>
   <trd lng="be">&c_s;&c_l;&c_a;&c_n;&c_je;&c_ch;&c_n;&c_ib;&c_k;</trd>
   <trd lng="de">Sonnenblume</trd>
   <trd lng="es">girasol</trd>
   <trd lng="he">&het;&mem;&nun;&yod;&tav;</trd>
   <trd lng="hu">napraforg&oacute;</trd>
   <trd lng="nl">zonnebloem</trd>
   <trd lng="fr">tournesol</trd>
   <trd lng="pl">s&lstroke;onecznik</trd>
   <trdgrp lng="ru">
     <trd>&c_p;&c_o;&c_d;&c_s;&c_o;&c_l;&c_n;&c_u;&c_h;</trd>,
 
<trd>&c_p;&c_o;&c_d;&c_s;&c_o;&c_l;&c_n;&c_je;&c_ch;&c_n;&c_i;&c_k;</trd>
   </trdgrp>
   <trd lng="sv">solros</trd>
   <trd lng="tr">ay&ccedil;i&ccedil;e&gbreve;i</trd>
   <trd lng="vo">solaflol</trd>
</drv>

<drv mrk="sun.0sistemo">
   <kap><tld/>sistemo</kap>
   <uzo tip="fak">AST</uzo>
   <snc>
     <dif>
       <ref tip="prt" cel="sun.0o.stelo"><tld lit="S"/>o</ref> kune kun la
       &gcirc;in &ccirc;irka&ubreve;antaj astroj.
     </dif>
   </snc>
   <ref tip="malprt" cel="galaksi.0o">galaksio</ref>
   <trd lng="be">&c_s;&c_o;&c_n;&c_je;&c_ch;&c_n;&c_a;&c_ja;
&c_s;&c_y;&c_s;&c_t;&c_e;&c_m;&c_a;</trd>
   <trd lng="de">Sonnensystem</trd>
   <trd lng="he">&mem;&ayin;&resh;&kaf;&tav; &shin;&mem;&shin;</trd>
   <trd lng="hu">naprendszer</trd>
   <trd lng="fr">syst&egrave;me <ind>solaire</ind></trd>
   <trd lng="nl">zonnestelsel</trd>
   <trd lng="pl">uk&lstroke;ad <ind>s&lstroke;oneczny</ind></trd>
   <trd lng="ru">&c_s;&c_o;&c_l;&c_n;&c_je;&c_ch;&c_n;&c_a;&c_ja;
&c_s;&c_i;&c_s;&c_t;&c_je;&c_m;&c_a;</trd>
</drv>
</art>
