  <trd lng="he">&mem;&ayin;&resh;&kaf;&tav; &shin;&mem;&shin;</trd>
  <trd lng="hu">naprendszer</trd>
  <trd lng="fr">syst&egrave;me <ind>solaire</ind></trd>
  <trd lng="fy">sinnestelsel</trd>
  <trd lng="id">tata surya</trd>
  <trd lng="io">sunala sistemo</trd>
  <trd lng="it">sistema <ind>solare</ind></trd>
  <trd lng="ja">&#x592A;&#x967D;&#x7CFB;  <pr>&#x305F;&#x3044;&#x3088;&#x3046;&#x3051;&#x3044;</pr></trd>
  <trd lng="nl">zonnestelsel</trd>
  <trd lng="pl">uk&lstroke;ad <ind>s&lstroke;oneczny</ind></trd>
  <trd lng="pt">sistema solar</trd>
  <trd lng="ro">sistemul solar</trd>
  <trd lng="ru">&c_s;&c_o;&c_l;&c_n;&c_je;&c_ch;&c_n;&c_a;&c_ja; &c_s;&c_i;&c_s;&c_t;&c_je;&c_m;&c_a;</trd>
  <trd lng="sk">slne&ccaron;n&aacute; s&uacute;stava</trd>
  <trd lng="uk">&c_s;&c_o;&c_n;&c_ja;&c_ch;&c_n;&c_a; &c_s;&c_i;&c_s;&c_t;&c_je;&c_m;&c_a;</trd>
