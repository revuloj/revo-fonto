<drv mrk="sun.0umi">
  <kap><tld/>umi</kap>
  <snc mrk="sun.0umi.KOMUNE">
    <dif>
      Elmeti al <tld/>lumo por varmigi, sekigi...:
      <ekz>
        &Uacute;rsula memoris la solan aferon nekutiman, kiun Fernanda
        faris tiutage: <tld/>umi la matojn de la infanoj
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de
        Diego</aut>,
        <vrk>Cent
        Jaroj da Soleco</vrk>, <lok>1992</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_s;&c_t;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol; &c_n;&c_a; &c_s;&c_o;&c_n;&c_c;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>posar al sol</trd>,
    <trd>solejar</trd>,
    <trd>assolellar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>oh&rcaron;&iacute;vat na slunci</trd>,
    <trd>opalovat</trd>,
    <trd>slunit se</trd>
  </trdgrp>
  <trd lng="fr">exposer au soleil</trd>
  <trd lng="it">esporre al <ind>sole</ind></trd>
  <trdgrp lng="sk">
    <trd>opa&lcaron;ova&tcaron; sa</trd>,
    <trd>slni&tcaron; sa</trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_i;&c_s;&c_t;&c_a;&c_v;&#x43B;&#x44F;&c_t;&c_i; &#x43D;&c_a; &c_s;&#x43E;&#x43D;&c_c;&c_je;</trd>
</drv>

<drv mrk="sun.0umigxi">
  <kap><tld/>umi&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sun.0umigxi.KOMUNE">
    <dif>
      Elmeti&gcirc;i al <tld/>radiado:
      <ekz>
        onklo Pan kondukis la junan bovinon sur la vila&gcirc;an 
        placon por <tld/>umi&gcirc;i
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_g;&c_a;&c_r;&c_a;&c_c;&c_mol; <klr>(&c_n;&c_a; &c_s;&c_o;&c_n;&c_c;&c_y;)</klr></trd>
  <trdgrp lng="fr">
    <trd>l&eacute;zarder<klr> (au soleil)</klr></trd>,
    <trd>prendre un bain de <ind>soleil</ind></trd>
  </trdgrp>
</drv>

<drv mrk="sun.0sistemo">
  <kap><tld/>sistemo</kap>
  <snc mrk="sun.0sistemo.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="prt" cel="sun.0o.stelo"><tld lit="S"/>o</ref> kune kun
      la &gcirc;in &ccirc;irka&ubreve;antaj astroj.
    </dif>
  </snc>
  <ref tip="malprt" cel="galaksi.0o">galaksio</ref>
  <trd lng="be">&c_s;&c_o;&c_n;&c_je;&c_ch;&c_n;&c_a;&c_ja; &c_s;&c_y;&c_s;&c_t;&c_e;&c_m;&c_a;</trd>
  <trd
  lng="bo">&#xF49;&#xF72;&#xF0B;&#xF58;&#xF60;&#xF72;&#xF0B;&#xF41;&#xFB1;&#xF72;&#xF58;&#xF0B;&#xF62;&#xF92;&#xFB1;&#xF74;&#xF51;&#xF0B;</trd>
  <trd lng="ca">sistema <ind>sol</ind>ar</trd>
  <trd lng="cs">slune&ccaron;n&iacute; soustava</trd>
  <trd lng="de">Sonnensystem</trd>
  <trd lng="en">solar system</trd>
  <trd lng="es">sistema solar</trd>
  <trd lng="he">&mem;&ayin;&resh;&kaf;&tav; &shin;&mem;&shin;</trd>
  <trd lng="hu">naprendszer</trd>
  <trd lng="fr">syst&egrave;me <ind>solaire</ind></trd>
  <trd lng="fy">sinnestelsel</trd>
  <trd lng="id">tata surya</trd>
  <trd lng="io">sunala sistemo</trd>
  <trd lng="it">sistema <ind>solare</ind></trd>
  <trd lng="nl">zonnestelsel</trd>
  <trd lng="pl">uk&lstroke;ad <ind>s&lstroke;oneczny</ind></trd>
  <trd lng="pt">sistema solar</trd>
  <trd lng="ro">sistemul solar</trd>
  <trd lng="ru">&c_s;&c_o;&c_l;&c_n;&c_je;&c_ch;&c_n;&c_a;&c_ja; &c_s;&c_i;&c_s;&c_t;&c_je;&c_m;&c_a;</trd>
  <trd lng="sk">slne&ccaron;n&aacute; s&uacute;stava</trd>
  <trd lng="uk">&c_s;&#x43E;&#x43D;&#x44F;&c_ch;&#x43D;&c_a; &c_s;&c_i;&c_s;&c_t;&c_je;&#x43C;&c_a;</trd>
</drv>

<drv mrk="sun.ekster0sistema">
  <kap>ekster<tld/>sistema</kap>
  <snc mrk="sun.ekster0sistema.KOMUNE">
    <uzo tip="fak">AST</uzo>
    <dif>
      Situanta ekster <tld/>sistemo:
      <ekz>
        <ind>ekster<tld/>sistema planedo</ind>.
        <trd lng="be">&c_e;&c_k;&c_z;&c_a;&c_p;&c_l;&c_ja;&c_n;&c_e;&c_t;&c_a;</trd>
        <trdgrp lng="ca">
          <trd>planeta extrasolar</trd>,
          <trd>exoplaneta</trd>
        </trdgrp>
        <trd lng="en">exoplanet</trd>
        <trdgrp lng="it">
          <trd><ind>pianeta</ind> extrasolare</trd>,
          <trd>esopianeta</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;-&c_z;&c_a; <ind>&c_s;&c_o;&c_n;&c_je;&c_ch;&c_n;&c_a;&c_j;</ind> &c_s;&c_y;&c_s;&c_t;&c_e;&c_m;&c_a;&c_j;</trd>,
    <trd>&c_e;&c_k;&c_z;&c_a;-</trd>
  </trdgrp>
  <trd lng="ca">extrasolar</trd>
  <trd lng="it">extrasolare</trd>
</drv>

<drv mrk="sun.0subiro">
  <kap><tld/>subiro</kap>
  <snc mrk="sun.0subiro.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      <ref tip="malprt" cel="vesper.0o">Vespera</ref> malapero de la
      suno:
      <ekz>
        Kiam okazos la <tld/>subiro vendredon?
        <fnt>
          <vrk>Esperanto course for English speakers at Duolingo</vrk>
          <url
          ref="https://www.duolingo.com/dictionary/Esperanto/sunsubiro/b8fa55112d4888ab983aabad2be4fb03">sunsubiro</url>
        </fnt>
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_a;&c_h;&c_a;&c_d; <klr>(&c_s;&c_o;&c_n;&c_c;&c_a;)</klr></trd>
    <trd lng="cs">z&aacute;pad slunce</trd>
    <trd lng="en">sunset</trd>
    <trd lng="pl">zach&oacute;d s&lstroke;o&nacute;ca</trd>
    <trd lng="sk">z&aacute;pad slnka</trd>
  </snc>
  <trd lng="uk">&c_z;&c_a;&c_h;&c_ib;&c_d; &c_s;&#x43E;&#x43D;&c_c;&#x44F;</trd>
</drv>

