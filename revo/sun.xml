<drv mrk="sun.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sun.0o.nia">
  <mlg>&#x2609;</mlg>
    <dif>
      La <ref tip="super" cel="stel.0o.AST">stelo</ref>,
      &ccirc;irka&ubreve; kiu movi&gcirc;as la tero:
      <ekz>
        la <tld/>o rebrilas en la klara akvo de la rivero
        <fnt>
          <bib>F</bib>
          <lok>&FE; 42</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o levi&gcirc;is anta&ubreve; li
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 32:31</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la subiro de la <tld/>o profunda dormo falis sur Abramon
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 15:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi estas la plej feli&ccirc;a homo sub la <tld/>o
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        post vetero malbela lumas <tld/>o plej hela
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        e&ccirc; sur la <tld/>o trovi&gcirc;as makuloj
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">Sol</trd>
    <trd lng="de">Sonne</trd>
    <trdgrp lng="en">
      <trd><klr>the </klr><ind>Sun</ind></trd>,
      <trd>Sol<klr> (the Sun)</klr></trd>
    </trdgrp>
    <trd lng="he">&het;&mem;&he;</trd>
    <trd lng="hu">Nap</trd>
  </snc>
  <snc mrk="sun.0o.stelo">
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="dif" cel="stel.0o.AST">Stelo</ref>:
      <ekz>
        &gcirc;ia meza distanco de &gcirc;ia <tld/>o estas 0,3 astronomiaj unuoj (= Suno-Tero-distanco)
        <fnt>
          <bib>Monato</bib>
          <vrk>Du strangaj planedsistemoj</vrk>
          <lok>jaro 2001a, numero 7a, p. 20a</lok>
        </fnt>;
      </ekz>
      <ekz>
        planedoj, kiuj &ccirc;irka&ubreve;as <tld/>ojn for de nia <tld/>sistemo
        <fnt>
          <bib>LOdE</bib>
          <aut>Amri Wandel</aut>
          <vrk><url ref="&Sezonoj;2017/06/wandel/">Astrobiologio kaj la perspektivo de vivo
          ekstertera</url></vrk>
          <lok>20 junio 2017</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Henriko <nom>Dunant</nom> decidis turni sin al la levi&gcirc;anta politika <tld/>o, al la glora
        Napoleono la III-a
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Zentralgestirn</trd>
    <trd lng="hu">csillag</trd>
    <trdgrp lng="ca">
      <trd>estel</trd>,
      <trd>astre</trd>
    </trdgrp>
    <trd lng="en">star<klr> (n) (sun)</klr></trd>
  </snc>
  <snc mrk="sun.0o.radioj">
    <dif>
      <tld lit="S"/>lumo:
      <ekz>
        matena <tld/>o
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la unua radio de la <tld/>o penetris en la &ccirc;ambron
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la <tld/>o brilantaj literoj
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        radio de la <tld/>o trarompis la nubon
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        sub la kariba <tld/>o
        <fnt>
          <bib>ChV</bib>
          <lok>19</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">llum solar</trd>
    <trd lng="de">Sonnenstrahlen</trd>
    <trd lng="en">sun<klr> (n) (weather)</klr></trd>
    <trd lng="es">rayos solares</trd>
    <trd lng="hu">napf&eacute;ny</trd>
  </snc>
  <!-- SUPERFLUA DIFINO
  <snc mrk="sun.0o.figuro">
    <dif>
      <ref tip="super" cel="figur.0o">Figuro</ref> simbolanta la
      <tld/>on.
