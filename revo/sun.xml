<drv mrk="sun.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="sun.0umi.KOMUNE">
    <dif>
      Elmeti sin a&ubreve; ion al <tld/>lumo por varmigi, sekigi...:
      <ekz>
        <nom>&Uacute;rsula</nom> memoris la solan aferon nekutiman, kiun Fernanda
        faris tiutage: <tld/>umi la matojn de la infanoj
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de
        Diego</aut>,
        <vrk>Cent
        Jaroj da Soleco</vrk>, <lok>1992</lok></fnt>;
      </ekz>
      <ekz>
        vizitis fonton kaj akvofalon de rivero <nom>So&ccirc;a</nom> kaj <tld/>umis apud vigla kaj la&ubreve;ta
        rivereto
        <fnt>
          <bib>LOdE</bib>
          <aut>Vanja Radovanovi&ccirc;</aut>
          <vrk><url ref="&LOdE;74-lode.htm">Montkabana Renkonti&gcirc;o</url></vrk>
          <lok>2000. No 12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="ban.sun0i">sunbani</ref>
  </refgrp>
  <trd lng="be">&c_v;&c_y;&c_s;&c_t;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol; &c_n;&c_a; &c_s;&c_o;&c_n;&c_c;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>posar al sol</trd>,
    <trd>solejar</trd>,
    <trd>assolellar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>oh&rcaron;&iacute;vat na slunci</trd>,
    <trd>opalovat</trd>,
    <trd>slunit se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sonnen</trd>,
    <trd>in die <ind>Sonne</ind> legen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>expose to the sun<klr> (vt, vi)</klr></trd>,
    <trd>put in the sun<klr> (vt, vi)</klr></trd>,
    <trd>sun<klr> (vi)</klr></trd>
  </trdgrp>
  <trd lng="fr">exposer au soleil</trd>
  <trdgrp lng="id">
    <trd><ind>mandi</ind> matahari</trd>,
    <trd>ber<ind>jemur</ind> (di bawah matahari)</trd>
  </trdgrp>
  <trd lng="it">esporre al <ind>sole</ind></trd>
  <trd lng="ja">&#26085;&#20809;&#12395;&#12373;&#12425;&#12377;  <pr>&#12395;&#12387;&#12371;&#12358;&#12395;&#12373;&#12425;&#12377;</pr></trd>
  <trd lng="pl">opala&cacute; na  <ind>s&lstroke;o&nacute;cu</ind></trd>
  <trdgrp lng="sk">
    <trd>opa&lcaron;ova&tcaron; sa</trd>,
    <trd>slni&tcaron; sa</trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_i;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_i; &c_n;&c_a; &c_s;&c_o;&c_n;&c_c;&c_je;</trd>
</drv>

<drv mrk="sun.0umigxi">
  <kap><tld/>umi&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sun.0umigxi.KOMUNE">
    <dif>
      Elmeti&gcirc;i al <tld/>radiado:
      <ekz>
        onklo <nom>Pan</nom> kondukis la junan bovinon sur la vila&gcirc;an
        placon por <tld/>umi&gcirc;i
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>;
      </ekz>
      <ekz>
        la planto &scirc;atas <tld/>umi&gcirc;i dum longa tempo
        <fnt>
          <bib>Kon11</bib>
          <aut>Bira FEHERA</aut>
          <vrk>Kanabo, aventuro en Ameriko</vrk>
          <lok>n-ro 6, junio 2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_g;&c_a;&c_r;&c_a;&c_c;&c_mol; <klr>(&c_n;&c_a; &c_s;&c_o;&c_n;&c_c;&c_y;)</klr></trd>
  <trdgrp lng="de">
    <trd>sich <ind>sonnen</ind></trd>,
    <trd>sonnenbaden</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>be exposed to the sun</trd>,
    <trd>be put in the sun</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>l&eacute;zarder<klr> (au soleil)</klr></trd>,
    <trd>prendre un bain de <ind>soleil</ind></trd>
  </trdgrp>
  <trd lng="pl">opala&cacute; si&eogonek;</trd>
</drv>

<drv mrk="sun.0sistemo">
  <kap><tld/>sistemo</kap>
  <snc mrk="sun.0sistemo.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="prt" cel="sun.0o.stelo"><tld lit="S"/>o</ref> kune kun
      la &gcirc;in &ccirc;irka&ubreve;antaj astroj:
      <ekz>
        la planedoj de la <tld/>sistemo
        <fnt>
          <bib>VaK</bib>
          <lok>Dekunua &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malkovrado de miloj da planedoj for de nia <tld/>sistemo
        <fnt>
          <bib>Kon11</bib>
          <lok>E&hcirc;oj</lok>
        </fnt>;
      </ekz>
      <ekz>
        en aliaj <tld/>sistemoj <klr>[&#8230;]</klr> ver&scirc;ajne trovi&gcirc;as planedoj kun vivo
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
