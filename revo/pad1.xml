<vortaro>
  <art mrk="$Id: pad1.xml,v 1.2 2023/08/14 06:31:38 afido Exp $">
    <kap>
      <rad>Pad</rad>/o
    </kap>

<drv mrk="pad1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="pad1.0o.rivero">
    <dif>
      <ref tip="lst" lst="voko:riveroj" cel="river.0o.GEOG">Rivero</ref> fluanta
      en norda <ref tip="malprt" cel="ital.I0ujo">Italujo</ref> i.a. tra Torino
      en orienta direkto &gcirc;is Adriatiko:
      <ekz>
        la a&ubreve;to <klr>[&#8230;]</klr> ruli&gcirc;is &gcirc;is la rivero <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2013/011031.php"
              >Hundo &ccirc;e la fundo</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        post milionoj da jaroj <klr>[&#8230;]</klr> la fekunda <tld/>a Ebena&jcirc;o (a&ubreve; Valo de
        <tld/>o) malaperos, kunpremi&gcirc;inte inter Apeninoj kaj Alpoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2013/010777.php"
              >&Gcirc;is la tero tremas</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Po <klr>(Fluss)</klr></trd>
    <trd lng="en">Po <klr>(river)</klr></trd>
    <trd lng="fr">P&ocirc;</trd>
    <trd lng="it">Po</trd>
    <trd lng="lat">Padus</trd>
    <trd lng="ru">&c_P;&c_o; <ind>(&c_r;&c_je;&c_k;&c_a;)</ind></trd>
  </snc>
  <ref tip="hom" cel="pad.0o">pado (vojeto)</ref>
</drv>

  </art>

