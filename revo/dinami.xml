<drv mrk="dinami.flu0">
  <kap>flu<tld/>o,
  <var><kap>fluo<tld/>o</kap></var>,
  <var><kap>fluido<tld/>o</kap></var>
  </kap>
  <snc mrk="dinami.flu0.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Bran&ccirc;o de fiziko studanta la movi&gcirc;ojn de nesolidaj
      &mdash; tio estas flua&jcirc;aj a&ubreve; gasaj &mdash; substancoj:
      <ekz>
        <klr>...</klr> studi la kompleksan fluo<tld/>on <klr>...</klr> 
        kiam la maso venas subite al stagno
        <fnt>
          <aut>WikiTrans</aut>,
          <vrk><url ref="http://epo.wikitrans.net/%C5%9Cokondo">
          &Scirc;okondo</url></vrk>,
          <lok>2010-03-07</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_h;&c_i;&c_d;&c_r;&c_o;&c_g;&c_a;&c_z;&c_o;&c_d;&c_i;&c_n;&c_a;&c_m;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_je;&c_h;&c_a;&c_n;&c_i;&c_k;&c_a; &c_n;&c_a; &c_f;&c_l;&c_u;&c_i;&c_d;&c_i;&c_t;&c_je;</trd>
  </trdgrp>
  <trd lng="ca">din&agrave;mica de fluids</trd>
  <trd lng="fr">dynamique des fluides</trd>
  <trd lng="it">fluidodinamica</trd>
  <trdgrp lng="nl">
    <trd>vloeistofdynamica</trd>,
    <trd>vloeistofmechanica</trd>
  </trdgrp>
</drv>

<drv mrk="dinami.varmo0o">
  <kap>varmo<tld/>o</kap>
  <snc mrk="dinami.varmo0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Parto de <ref tip="malprt" cel="fizik.0o.FIZ">fiziko</ref>
      studanta
      la energiajn kaj materiajn &scirc;an&gcirc;ojn en sistemoj
    </dif>
    <ref tip="sin" cel="termodinamik.0o">termodinamiko</ref>
  </snc>
  <trd lng="bg">&c_t;&c_je;&c_r;&c_m;&c_o;&c_d;&c_i;&c_n;&c_a;&c_m;&c_i;&c_k;&c_a;</trd>
  <trd lng="ca">termodin&agrave;mica</trd>
  <trd lng="fr">thermodynamique</trd>
  <trd lng="it">termodinamica</trd>
  <trd lng="nl">thermodynamica</trd>
</drv>
