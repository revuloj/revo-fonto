<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: silogi.xml,v 1.16 2018/02/21 21:10:23 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>silogism</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="silogi.0o">
  <kap><tld/>o</kap>
  <snc mrk="silogi.0o.LOG">
    <dif>
      Logika <ref tip="super" cel="rezon.0o.LOG">rezono</ref>,
      per kiu oni tiras aserton el alia aserto pere de tria aserto:
      <ekz>
        mi eklernis pri <tld/>oj nur dek tri jarojn post &ccirc;i tio
        <fnt><aut>B. Vaha</aut>, <vrk>Adolesko</vrk>, 
        <lok>1987</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="nu.0">nu</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_l;&c_ja;&c_g;&c_ib;&c_z;&c_m;</trd>
  <trd lng="bg">&c_s;&c_i;&c_l;&c_o;&c_g;&c_i;&c_z;&c_malmol;&c_m;</trd>
  <trd lng="ca">sil&middot;logisme</trd>
  <trd lng="cs">sylogizmus</trd>
  <trd lng="de">Syllogismus</trd>
  <trd lng="en">syllogism</trd>
  <trd lng="fr">syllogisme</trd>
  <trd lng="hu">szillogizmus</trd>
  <trd lng="it">sillogismo</trd>
  <trdgrp lng="nl">
    <trd>syllogisme</trd>,
    <trd>sluitrede</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_i;&c_l;&c_l;&c_o;&c_g;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">syllogizmus</trd>
</drv>
</art>
