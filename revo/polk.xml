<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>polk</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="polk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="polk.0o.danco">
    <dif>
      Para <ref tip="super" cel="danc.0o">danco</ref> bohem-devena,
      du-takta, hopema,
      populara en E&ubreve;ropo kaj Nord-Ameriko en la 19a jarcento.
    </dif>
  </snc>
  <snc mrk="polk.0o.muzikajxo">
    <dif>
      Muzika&jcirc;o ta&ubreve;ga por <tld/>o-danco:
      <ekz>
        trankvili&gcirc;u, Timoteo, kaj ellasu ree el via violono
        gajan <tld/>on
        <fnt>
          <aut>A. Kivi, trad. H. Hall</aut>
          <vrk><url
          ref="http://www.gutenberg.org/files/28968/28968-h/28968-h.htm">
          La Botistoj</url></vrk>
          <lok>1919</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_o;&c_l;&c_mol;&c_k;&c_a;</trd>
  <trd lng="ca">polca</trd>
  <trd lng="de">Polka</trd>
  <trd lng="en">polka</trd>
  <trd lng="es">polca</trd>
  <trd lng="fr">polka</trd>
  <trd lng="hu">polka</trd>
  <trdgrp lng="it">
    <trd>polka</trd>,
    <trd>polca</trd>
  </trdgrp>
  <trd lng="nl">polka</trd>
  <trd lng="pl">polka</trd>
  <trd lng="pt">polca</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_mol;&c_k;&c_a;</trd>
  <trd lng="sv">polka</trd>
</drv>
</art>
