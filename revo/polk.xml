<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>polk</rad>/o
</kap>
<drv mrk="polk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="polk.0o.danco">
    <dif>
      Para <ref tip="super" cel="danc.0o">danco</ref> bohem-devena,
      du-takta, hopema,
      populara en E&ubreve;ropo kaj Nord-ameriko en la 19a jarcentoj.
    </dif>
  </snc>
  <snc mrk="polk.0o.muzikajxo">
    <dif>
      Muzika&jcirc;o ta&ubreve;ga por <tld/>o-danco.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_o;&c_l;&c_mol;&c_k;&c_a;</trd>
  <trd lng="ca">polca</trd>
  <trd lng="de">Polka</trd>
  <trd lng="en">polka</trd>
  <trd lng="es">polca</trd>
  <trdgrp lng="fr">
    <trd>polka</trd>
  </trdgrp>
  <trd lng="hu">polka</trd>
  <trd lng="nl">polka</trd>
  <trd lng="pl">polka</trd>
  <trd lng="pt">polca</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_mol;&c_k;&c_a;</trd>
  <trd lng="sv">polka</trd>
</drv>
</art>
