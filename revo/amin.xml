<art mrk="$Id: amin.xml,v 1.9 2023/08/30 18:23:33 revo Exp $">
<kap>
  <rad>amin</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="amin.0o">
  <kap><tld/>o</kap>
  <snc mrk="amin.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Organika substanco derivita el amoniako, en kiu
      hidrogrenon anstata&ubreve;as karbaj kompona&jcirc;oj:
      <ekz>
        aranea veneno konsistas el miksa&jcirc;o de proteinoj, aminoj kaj 
        polipeptidoj
        <fnt>
          <aut>E. Nieuwenhuys, trad. L. De Cooman</aut>,
          <vrk><url ref="http://www.xs4all.nl/~ednieuw/Spiders/InfoNed/Veneniloj_kaj_veneno.html">
          Araneo</url></vrk>,
          <lok>[1999?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_ib;&c_n;</trd>
  <trd lng="cs">amin</trd>
  <trd lng="en">amine</trd>
  <trd lng="fr">amine</trd>
  <trd lng="ja">&#x30A2;&#x30DF;&#x30F3;</trd>
  <trd lng="pl">amin</trd>
  <trd lng="sk">am&iacute;n</trd>
  <trd lng="uk">&#x430;&#x43C;&#x456;&#x43D;</trd>
</drv>
</art>

