<vortaro>
<art mrk="$Id: odekolon.xml,v 1.17 2024/01/30 16:09:13 revo Exp $">
<kap>
  <rad>odekolon</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="odekolon.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="akv.kolonja_0o">Kolonja akvo</ref> a&ubreve; iu el &gcirc;iaj
      imita&jcirc;oj:
      <ekz>
        Pa&ubreve;lo <nom>Petrovi&ccirc;</nom> frotis sian frunton per <tld/>o kaj fermis la
        okulojn
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 24a, p.
        171a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_e;&c_k;&c_a;&c_l;&c_jo;&c_n;</trd>
  <trd lng="cs">kol&iacute;nsk&aacute; voda</trd>
  <trd lng="de">K&ouml;lnisch Wasser</trd>
  <trdgrp lng="en">
    <trd>cologne</trd>,
    <trd>eau de cologne</trd>,
    <trd>toilet water</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>eau de Cologne</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&ouml;lni</trd>,
    <trd>k&ouml;lniv&iacute;z</trd>,
  </trdgrp>
  <trdgrp lng="nl">
    <trd>eau de cologne</trd>
  </trdgrp>
  <trd lng="pl">woda <ind>kolo&nacute;ska</ind></trd>
  <trdgrp lng="pt">
    <trd>&aacute;gua-de-col&ocirc;nia</trd>,
    <trd>col&ocirc;nia</trd>
    <trd>&aacute;gua-da-col&ocirc;nia</trd>
    <trd>&aacute;gua-de-cheiro</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_d;&c_je;&c_k;&c_o;&c_l;&c_o;&c_n;</trd>
  <trd lng="sk">kol&iacute;nska voda</trd>
  <trdgrp lng="zh">
    <trd>&#x82B1;&#x9732;&#x6C34; <pr>hu&#x101;l&#xF9;shu&#x1D0;</pr></trd>,
    <trd>&#x79D1;&#x9686;&#x9999;&#x6C34; <pr>k&#x113;l&#xF3;ngxi&#x101;ngshu&#x1D0;</pr></trd>
  </trdgrp>
</drv>
</art>
