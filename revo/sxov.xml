<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>&scirc;ov</rad>/i
    </kap>

    <drv mrk="sxov.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="sxov.0i.glite">
	<dif>
	  Pu&scirc;i ion glitigante &gcirc;in sur supra&jcirc;o, sur kiu
	  &gcirc;i ku&scirc;as, por anta&ubreve;enmeti, proksimigi, montri
	  &gcirc;in:
	  <ekz>
	    <tld/>i paka&jcirc;on sur la planko<fnt>B</fnt>;
	  </ekz><ekz>
	    <tld/>i la fenestron, la pordon;
	  </ekz><ekz>
	    <uzo tip="stl">FIG</uzo>
	    la naturo, kiu longan tempon batalis kontra&ubreve; ni,
	    batalas nun por ni kaj <tld/>as nin anta&ubreve;en<fnt>Z</fnt>
	    <klr>(akcelas, favoras, progresigas)</klr>;
	  </ekz><ekz>
	    katastrofa ter<tld/>i&gcirc;o;
	  </ekz><ekz>
	    <tld/>i&gcirc;ado
	    (<ref tip="sin" cel="ramp.0i">rampi</ref>)
	    de limako<fnt>Z</fnt>;
	  </ekz><ekz>
	    al<tld/>i se&gcirc;on al iu;
	  </ekz><ekz>
	    sur<tld/>i kapu&ccirc;on sur la kapon<fnt>Z</fnt>;
	  </ekz><ekz>
	    sur<tld/>i la &ccirc;apelon sur la viza&gcirc;on;
	  </ekz><ekz>
	    la pordokurtenoj dis<tld/>i&gcirc;is;
	  </ekz><ekz>
	    liaj mallar&gcirc;aj brovoj kun<tld/>i&gcirc;is<fnt>Z</fnt>;
	  </ekz><ekz>
	    sin ien prem<tld/>i.
	  </ekz>
	</dif>
	<trd lng="pt">empurrar</trd>
	<trdgrp lng="ru">
	  <trd>&c_s;&c_d;&c_v;&c_i;&c_g;&c_a;&c_t;&c_mol;</trd>,
	  <trd>&c_s;&c_d;&c_v;&c_i;&c_n;&c_u;&c_t;&c_mol;</trd>
	</trdgrp>
      </snc>
      <snc mrk="sxov.0i.meti">
	<dif>
	  Milde, zorge, singarde a&ubreve; ka&scirc;e meti:
	  <ekz>
	    <tld/>i sian
	    manon en poton<fnt>Z</fnt>,
	  </ekz><ekz>
	    en la po&scirc;on<fnt>B</fnt>;
	  </ekz><ekz>
	    ne <tld/>u la nazon en fremdan vazon
	    <fnt><bib>PrV</bib></fnt>;
	  </ekz><ekz>
	    kie diablo ne povas, tien virinon li <tld/>as
	    <fnt><bib>PrV</bib></fnt>;
	  </ekz><ekz>
	    provu <tld/>i al li en la manon kelkan sumon da mono<fnt>K</fnt>
	  </ekz>
	</dif>
      </snc>
      <trdgrp lng="be">
	<trd>&c_s;&c_u;&c_n;&c_u;&c_c;&c_mol;</trd>, 
	<trd>&c_s;&c_s;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>, 
	<trd>&c_s;&c_o;&c_v;&c_a;&c_c;&c_mol;</trd>, 
	<trd>&c_p;&c_h;&c_a;&c_c;&c_mol;</trd>, 
	<trd>&c_r;&c_u;&c_h;&c_a;&c_c;&c_mol;</trd>
      </trdgrp>
      <trd lng="en">shove</trd>
      <trd lng="nl">schuiven</trd>
      <trd lng="pt">enfiar</trd>
      <trdgrp lng="ru">
	<trd>&c_s;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
	<trd>&c_s;&c_u;&c_n;&c_u;&c_t;&c_mol;</trd>
      </trdgrp>
    </drv>

    <drv mrk="sxov.de0i">
      <kap>de<tld/>i</kap>
      <snc  mrk="sxov.de0i.KOMUNE">
	<dif>
	  Movi flanken je certa distanco <tld/>ante:
	  <ekz>
	    Kial do vi de<tld/>as vian se&gcirc;on? Al ni estos pli bone sidi
	    proksime unu de la alia.
	    <fnt><bib>Revizoro</bib>, <lok>4:12.</lok></fnt>.
	  </ekz>
	</dif>
	<trd lng="ru">&c_o;&c_t;&c_o;&c_d;&c_v;&c_i;&c_n;&c_u;&c_t;&c_mol;</trd>
      </snc>
      <snc  mrk="sxov.de0i.TIP">
	<uzo tip="fak">TIP</uzo>
	<dif>
	  Komenci linion je kelka distanco de la mar&gcirc;eno:
	  <ekz>
	    de<tld/>i la unuan linion de <ref tip="malprt"
	      cel="aline.0o.tekstopeco">alineo<sncref/></ref>
	    <fnt><bib>PIV2</bib>, <lok>p. 1117.</lok></fnt>.
	  </ekz><ekz>
	    de<tld/>i enen, eksteren.
	  </ekz>
	</dif>
	<ref tip="vid" cel="margxen.krom0o">krommar&gcirc;eno</ref>
	<trd lng="en">indent</trd>
	<trd lng="ru">&c_o;&c_t;&c_s;&c_t;&c_u;&c_p;&c_i;&c_t;&c_mol;<klr tip="amb"> (&c_v; &c_k;&c_r;&c_a;&c_s;&c_n;&c_o;&c_j; &c_s;&c_t;&c_r;&c_o;&c_k;&c_je;)</klr></trd>
      </snc>
    </drv>

    <drv mrk="sxov.el0i">
      <kap>el<tld/>i</kap>
      <snc>
	<dif>
	  <tld lit="&Scirc;"/>ante eligi ion el io:
	  <ekz>
	    el<tld/>i la langon<fnt>Z</fnt>;
	  </ekz><ekz>
	    birdeto el<tld/>as la kapeton el la nesto<fnt>Z</fnt>.
	  </ekz>
	</dif>
	<trd lng="nl">uitsteken</trd>
      </snc>
      <snc>
	<uzo tip="stl">FIG</uzo>
	<dif>
	  Subite prezenti ion:
	  <ekz>
	    el<tld/>i <klr>(meti)</klr> demandon;
	  </ekz><ekz>
	    ka&scirc;u kiom vi povos, mensogo sin el<tld/>os
	    <klr>(montros)</klr>
	    <fnt><bib>PrV</bib></fnt>;
	  </ekz><ekz>
	    miljaroj ne povis, minuto el<tld/>is
	    <fnt><bib>PrV</bib></fnt>;
	  </ekz><ekz>
	    tiaj personoj diras malveron kaj post unu refutita
	    mal&gcirc;usta&jcirc;o balda&ubreve; el<tld/>us dek
	    aliajn<fnt>Z</fnt>.
	  </ekz>
	</dif>
      </snc>
      <trdgrp lng="be">
	<trd>&c_v;&c_y;&c_s;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>, 
	<trd>&c_v;&c_y;&c_c;&c_ja;&c_g;&c_v;&c_a;&c_c;&c_mol;</trd>
      </trdgrp>
      <trdgrp lng="ru">
	<trd>&c_v;&c_y;&c_s;&c_o;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
	<trd>&c_v;&c_y;&c_s;&c_u;&c_n;&c_u;&c_t;&c_mol;</trd>,
	<trd>&c_v;&c_y;&c_d;&c_v;&c_i;&c_g;&c_a;&c_t;&c_mol;</trd>,
	<trd>&c_v;&c_y;&c_d;&c_v;&c_i;&c_n;&c_u;&c_t;&c_mol;</trd>
      </trdgrp>
    </drv>

    <drv mrk="sxov.en0i">
      <kap>en<tld/>i</kap>
      <snc>
	<dif>
	  Enigi <tld/>ante:
	  <ekz>
	    en<tld/>i paperujon en sian po&scirc;on;
	  </ekz><ekz>
	    li de&scirc;iris unu rozon kaj en<tld/>is &gcirc;in sur la
	    &ccirc;apelon<fnt>Z</fnt>.
	  </ekz>
	</dif>
      </snc>
      <snc>
	<dif>
	  Delikate a&ubreve; ka&scirc;e meti:
	  <ekz>
	    en<tld/>i la kapon en
	    kusenon<fnt>B</fnt>;
	  </ekz><ekz>
	    estas necese en<tld/>i <klr>(enkonduki)</klr> en nian propagandon
	    praktikan spiriton;
	  </ekz><ekz>
	    &scirc;i reen<tld/>i&gcirc;is <klr>(reglitis,
	      reenprofundi&gcirc;is)</klr> en sian melankolion;
	  </ekz><ekz>
	    li en<tld/>i&gcirc;is <klr>(en&scirc;teli&gcirc;is)</klr> en la
	    &ccirc;ambron.
	  </ekz>
	</dif>
      </snc>
      <trdgrp lng="be">
	<trd>&c_u;&c_s;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>, 
	<trd>&c_z;&c_a;&c_s;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
      </trdgrp>
      <trd lng="nl">instoppen</trd>
      <trdgrp lng="ru">
	<trd>&c_v;&c_s;&c_o;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
	<trd>&c_v;&c_s;&c_u;&c_n;&c_u;&c_t;&c_mol;</trd>,
	<trd>&c_v;&c_d;&c_v;&c_i;&c_g;&c_a;&c_t;&c_mol;</trd>,
	<trd>&c_v;&c_d;&c_v;&c_i;&c_n;&c_u;&c_t;&c_mol;</trd>
      </trdgrp>
    </drv>

    <drv mrk="sxov.for0i">
      <kap>for<tld/>i</kap>
      <snc>
	<dif>
	  Forigi <tld/>ante:
	  <ekz>
	    ne for<tld/>u la antikvajn limojn, kiujn faris
	    viaj patroj<fnt>Z</fnt>;
	  </ekz><ekz>
	    kiu okazon for<tld/>as (f: rifuzas, ne profitas,
	    preterlasas), &gcirc;in jam ne retrovas
	    <fnt><bib>PrV</bib></fnt>;
	  </ekz><ekz>
	    la kurteno for<tld/>eti&gcirc;is<fnt>Z</fnt>.
	  </ekz>
	</dif>
      </snc>
      <trdgrp lng="be">
	<trd>&c_a;&c_d;&c_s;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>, 
	<trd>&c_a;&c_d;&c_s;&c_u;&c_n;&c_u;&c_c;&c_mol;</trd>
      </trdgrp>
      <trd lng="nl">opzij <ind>schuiven</ind></trd>
    </drv>

    <drv mrk="sxov.kun0i">
      <kap>kun<tld/>i</kap>
      <snc  mrk="sxov.kun0i.KOMUNE">
	<dif>
	  Interproksigi <tld/>ante:
	  <ekz>
	    la mallar&gcirc;aj brovoj de Ludovikino Zminska iom kun<tld/>i&gcirc;is
	    <fnt><bib>Marta</bib></fnt>.
	  </ekz>
	</dif>
	<trd lng="ru">&c_s;&c_d;&c_v;&c_i;&c_n;&c_u;&c_t;&c_mol;</trd>
      </snc>
      <snc  mrk="sxov.kun0i.TIP">
	<uzo tip="fak">TIP</uzo>
	<dif>
	  Iom proksimigi apudajn literojn por ke la interliteraj
	  distancoj aspektu pli egalaj:
	  <ekz>
	    pro kun<tld/>ado <em>VAVAVA</em> povas bezoni malpli da
	    spaco ol <em>VVVAAA</em>
	    <fnt><bib>PIV2</bib>, <lok>p. 1118.</lok></fnt>.
	  </ekz>
	</dif>
	<trd lng="en">kern</trd>
	<trdgrp lng="ru">
	  <trd>&c_p;&c_o;&c_d;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol;<klr> (&c_l;&c_i;&c_t;&c_je;&c_r;&c_y;)</klr></trd>,
	  <trd>&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; <ind>&c_k;&c_je;&c_r;&c_n;&c_i;&c_n;&c_g;</ind></trd>
	</trdgrp>
      </snc>
    </drv>

    <drv mrk="sxov.preter0igxitra0igxi">
      <kap>preter<tld/>i&gcirc;i, tra<tld/>i&gcirc;i</kap>
      <snc>
	<dif>
	  Delikate pasi preter, tra io:
	  <ekz>
	    ili ne atentis la virinon, kies
	    funebra vesto preter<tld/>i&gcirc;is anta&ubreve; ili<fnt>Z</fnt>;
	  </ekz><ekz>
	    &ccirc;i tiuj bildoj preter<tld/>i&gcirc;is anta&ubreve;
	    &scirc;ia animo<fnt>Z</fnt>;
	  </ekz><ekz>
	    malnovaj historioj tra<tld/>i&gcirc;is al &scirc;i tra la
	    menso kiel rapidemaj bildoj de magia lanterno<fnt>Z</fnt>.
	  </ekz>
	</dif>
	<rim>
	  <em>&scirc;ovi</em> montras &gcirc;enerale agon mildan, moderan,
	  delikatan, zorgan, singardan a&ubreve; ka&scirc;an, dum
	  <em>pu&scirc;i</em> montras prefere agon malmoderan, maldelikatan,
	  perfortan:
	  <ekz>
	    ni en<tld/>i&gcirc;is iom post iom en la amason kaj
	    atingis nerimarkite &gcirc;is la unuaj vicoj.
	  </ekz>
	  Kp:
	  <ekz>
	    ni trapu&scirc;is nin tra la amaso kaj rapide forkuris.
	  </ekz>
	</rim>
      </snc>
      <trd lng="be">&c_p;&c_r;&c_a;&c_s;&c_u;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>
      <trd lng="nl">doorschuiven</trd>
      <trdgrp lng="ru">
	<trd>&c_p;&c_r;&c_o;&c_s;&c_u;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
	<trd>&c_p;&c_r;&c_o;&c_d;&c_v;&c_i;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>
      </trdgrp>
    </drv>
  </art>
