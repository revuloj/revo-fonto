<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: eston.xml,v 1.29 2023/11/07 20:34:12 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>eston</rad>/o <fnt><bib>PIV1</bib></fnt>,
  <var><kap><rad var="est">est</rad>/o <fnt><bib>VivZam</bib></fnt></kap></var>
</kap>

<drv mrk="eston.0o">
  <kap><ofc>8</ofc>
    <tld/>o, <var><kap><tld var="est"/>o</kap></var>
  </kap>
  <snc mrk="eston.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="eston.E0ujo"><tld lit="E"/>ujo</ref>:
      <ekz>
        belgoj kaj <tld/>oj posedas elektronikan identeckarton
        <fnt>
          <bib>Monato</bib>
          <aut>pp</aut>
          <vrk><url ref="&Monato;/2011/010386.php">Estonio instruas Belgion</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="tr">
      <trd>Eston</trd>
      <trd>Estonyal&inodot;</trd>
    </trdgrp>
  </snc>
  <snc mrk="eston.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco:
      <ekz>
         por <tld/>oj, rusaj estroj devus sian samnaciulojn alvoki reen al sia patrujo
        <fnt>
           <aut>Helju Viires</aut>      
          <vrk><url ref="&Monato;/1993/A930901.php">Hejmen la rusoj ...?</url></vrk>
          <bib>Monato</bib>

          <lok>1993/09, p. 9</lok>
        </fnt>.
      </ekz>
      <ekz>
        en Litovio entute 83,5&#x202F;% de la lo&gcirc;antaro estas litovoj, en Estonio, tamen, <tld/>oj
        estas
        67,9&#x202F;%
        <fnt>
          <bib>Monato</bib>
          <aut>Last</aut>
          <vrk><url ref="&Monato;/2003/007971.html">Kiu kie lo&gcirc;as?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        por multaj <tld/>oj la monumento tamen simbolis ne liberigon, sed sovetian okupacion de la
        lando
        <fnt>
          <bib>Monato</bib>
          <aut>Kalle Kniivil&auml;</aut>
          <vrk><url ref="&Monato;/2007/009251.php">Monumenta krizo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en regno titolita &leftquot;Rusujo&rightquot;, la Rusoj sin kredis posedantoj kaj Letoj, <tld lit="E" var="est"/>oj a&ubreve;
        Poloj estis premataj
        <fnt>
          <bib>VivZam</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="et">eestlane</trd>
  </snc>
  <trd lng="be">&c_e;&c_s;&c_t;&c_o;&c_n;&c_je;&c_c;</trd>
  <trd lng="bg">&c_je;&c_s;&c_t;&c_o;&c_n;&c_je;&c_c;</trd>
  <trd lng="cs">Estonec</trd>
  <trd lng="de">Este</trd>
  <trd lng="en">Estonian</trd>
  <trd lng="es">estonio <klr tip="amb">(natural de Estonia)</klr></trd>
  <trd lng="fr">Estonien<klr> (subst.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x5B58;&#x5728;  <pr>&#x305D;&#x3093;&#x3056;&#x3044;</pr></trd>,
    <trd>&#x5B9F;&#x5728;  <pr>&#x3058;&#x3064;&#x3056;&#x3044;</pr></trd>,
    <trd>&#x751F;&#x5B58;  <pr>&#x305B;&#x3044;&#x305E;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">Est</trd>
  <trd lng="pl">Esto&nacute;czyk</trd>
  <trd lng="sk">Est&oacute;nec</trd>
  <trd lng="uk">&c_je;&c_s;&c_t;&#x43E;&#x43D;&c_je;&c_c;&#x44C;</trd>
  <trdgrp lng="zh">
    <trd>&#x611B;&#x6C99;&#x5C3C;&#x4E9E;&#x4EBA; <pr>&#xE0;ish&#x101;n&#xED;y&#xE0;r&#xE9;n</pr></trd>,
    <trd>&#x611B;&#x6C99;&#x5C3C;&#x4E9E;&#x4EBA; <pr>&#xE0;ish&#x101;n&#xED;y&#xE0;r&#xE9;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="eston.0a">
  <kap><tld/>a</kap>
  <snc mrk="eston.0a.rilatanta">
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato:
      <ekz>
        la <tld/>a &ccirc;efurbo <nom>Tallinn</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>LAST</aut>
          <vrk><url ref="&Monato;/2003/007971.html">Kiu kie lo&gcirc;as?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a lingvo
        <fnt>
          <bib>Monato</bib>
          <aut>Jorma Ahom&auml;ki</aut>
          <vrk><url ref="&Monato;/2004/008348.html">Nova vortarego en
          Estonio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        foriru la rusa armeo el <tld/>a teritorio
        <fnt>
           <aut>Helju Viires</aut>      
          <vrk><url ref="&Monato;/1993/A930901.php">Hejmen la rusoj ...?</url></vrk>
          <bib>Monato</bib>

          <lok>1993/09, p. 9</lok>
        </fnt>;
      </ekz>
      <ekz>
        rusiaj politikistoj admonis homojn bojkoti <tld/>ajn varojn, kaj reprezentantoj
        de por-registaraj junularaj organiza&jcirc;oj vizitadis vendejojn por kontroli &ccirc;u
        estas vendataj <tld/>a&jcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Kalle Kniivil&auml;</aut>
          <vrk><url ref="&Monato;/2007/009251.php">Monumenta krizo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="eston.0a.lingvo">
    <dif>
      La <tld/>a lingvo:
      <ekz>
        aliaj malpli grandaj lingvoj kiel la malta kaj la <tld/>a oficiali&gcirc;is en Bruselo
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2004/008449.html">Hispanio volas la katalunan, e&ubreve;skan
          kaj galegan kiel EU-lingvojn</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_s;&c_t;&c_o;&c_n;&c_s;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_je;&c_s;&c_t;&c_o;&c_n;&c_s;&c_k;&c_i;</trd>
  <trd lng="cs">estonsk&yacute;</trd>
  <trd lng="de">estnisch</trd>
  <trd lng="en">Estonian</trd>
  <trd lng="et">eesti keel</trd>
  <trd lng="fr">estonien<klr> (adj.)</klr></trd>
  <trd lng="nl">Ests</trd>
  <trd lng="no">estisk</trd>
  <trd lng="pl">esto&nacute;ski</trd>
  <trd lng="sk">est&oacute;nsky</trd>
  <trd lng="tr">Estonca</trd>
  <trd lng="uk">&c_je;&c_s;&c_t;&#x43E;&#x43D;&c_s;&#x44C;&#x43A;&c_i;&c_j;</trd>
</drv>

<drv mrk="eston.E0ujo">
  <kap><tld lit="E"/>ujo</kap>
  <mlg kod="ISO-3166">EE</mlg>
  <snc mrk="eston.E0ujo.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&Scirc;tato</ref> en nordorienta
      <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref> apud 
      <ref tip="vid" cel="mar.BaltaM0o">Balta Maro</ref>, 
      limanta en la sudo kun <ref tip="vid" cel="latv.L0ujo">Latvujo</ref>
      kaj en la oriento kun <ref tip="vid" cel="rus.R0aFederacio">Ruslanda Federacio</ref>
      kun la &ccirc;efurbo Talino:
      <ekz>
        Fino Elinjo <nom>Pahn</nom> el <tld lit="E"/>ujo kursgvidadas de 1930 en Svedujo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=10724">Cseh-metodo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="estoni.E0o">Estonio</ref>
  </snc>
  <trd lng="be">&c_E;&c_s;&c_t;&c_o;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_Je;&c_s;&c_t;&c_o;&c_n;&c_i;&c_ja;</trd>
  <trd lng="da">Estland</trd>
  <trd lng="de">Estland</trd>
  <trd lng="en">Estonia</trd>
  <trd lng="es">Estonia</trd>
  <trd lng="et">Eesti</trd>
  <trdgrp lng="fr">
    <trd>Estonie</trd>,
    <trd>R&eacute;publique d'Estonie</trd>
  </trdgrp>
  <trd lng="nl">Estland</trd>
  <trd lng="no">Estland</trd>
  <trd lng="pl">Estonia</trd>
  <trd lng="pt">Est&oacute;nia</trd>
  <trd lng="sv">Estland</trd>
  <trd lng="tr">Estonya</trd>
  <trd lng="uz">&c_E;&c_s;&c_t;&c_o;&c_n;&c_i;&c_ja;</trd>
  <trd lng="zh">&#x611B;&#x6C99;&#x5C3C;&#x4E9E; <pr>&#xE0;ish&#x101;n&#xED;y&#xE0;</pr></trd>
</drv>

</art>

