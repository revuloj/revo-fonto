<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>Lesb</rad>/o
</kap>
<drv mrk="lesb.0o.GEOG">
  <kap><tld lit="L"/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Insulo inter Grekujo kaj Turkujo. &Ccirc;efurbo: Mitileno.
    </dif>
  </snc>
  <trd lng="fr">Lesbos</trd>
  <trd lng="nl">Lesbos</trd>
</drv>
<drv mrk="lesb.l0o">
  <kap><tld lit="l"/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="lesb.l0anino.SOC"><tld/>anino</ref>:
      <ekz>
        kiajn &scirc;ancojn alportas PACS al ni, <tld lit="l"/>oj kaj gejoj?
        <fnt>
        <lok>Forumo, membrogazeto de LSG (Ligo de Samseksamaj
        Geesperantistoj), 1/2000</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>homosexuelle<klr tip="ind"> (subst.)</klr></trd>,
    <trd>lesbienne</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>lesbische</trd>,
    <trd>lesbienne</trd>
  </trdgrp>
</drv>
<drv mrk="lesb.l0anino">
  <kap><tld lit="l"/>anino</kap>
  <snc mrk="lesb.l0anino.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Lo&gcirc;antino de <tld/>o.
    </dif>
    <trd lng="fr">lesbienne<klr tip="ind"> (habitante de Lesbos)</klr></trd>
  </snc>
  <snc mrk="lesb.l0anino.SOC">
    <dif>
      <ref tip="dif" cel="seks.sam0emulino">Samseksemulino</ref>.
    </dif>
    <ref tip="vid" cel="gej.0o">gejo</ref>
    <trdgrp lng="fr">
      <trd>homosexuelle<klr tip="ind"> (subst.)</klr></trd>,
      <trd>lesbienne</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>lesbische</trd>,
      <trd>lesbienne</trd>
    </trdgrp>
  </snc>
</drv>
<drv mrk="lesb.l0a">
  <kap><tld lit="l"/>a</kap>
  <snc>
    <dif>
      Rilata al <tld lit="l"/>oj:
      <ekz>
        batali kontra&ubreve; anta&ubreve;ju&gcirc;oj kune kun gefratoj gejaj 
        kaj <tld lit="l"/>aj estus granda honoro por mi
        <fnt>
        <lok>Forumo, membrogazeto de LSG, 2/2000</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vivrilato inter geja a&ubreve; <tld lit="l"/>a paro nun 
        &ccirc;iurilate estos le&gcirc;e egala al tiu de geedza paro
        <fnt><aut>Gerrit Berveling</aut>:
        <vrk><url ref="&Monato;/007192.html">Edzi&gcirc;oj anka&ubreve; por samseksaj paroj</url></vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 12/2000</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="gej.0a">geja</ref>
  </snc>
  <trd lng="fr">lesbien</trd>
  <trd lng="nl">lesbisch</trd>
</drv>
</art>
