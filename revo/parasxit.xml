<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: parasxit.xml,v 1.6 2025/12/24 02:21:56 revo Exp $">
<kap>
  <rad>para&scirc;it</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="parasxit.0o">
  <kap><tld/>o</kap>
  <snc mrk="parasxit.0o.FUN">
    <dif>
      Mumiigisto preparanta kadavrojn per elpreno de interna&jcirc;oj:
      <ekz>
        &gcirc;ustadire ili similis malpurajn <tld/>ojn, kiuj distran&ccirc;as la
        mortintojn por enbalzamigo
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 11a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="balzam.en0igisto.FUN">enbalzamigisto</ref>,
      <ref cel="kolsxit.0o.FUN">kol&scirc;ito</ref>
    </refgrp>
  <trdgrp lng="en">
    <trd>paraschist&emacron;s <klr>(ancient Egyptian embalmer)</klr></trd>,
    <trd>embalmer <klr>(specifically a paraschist&emacron;s)</klr></trd>
  </trdgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_l;&c_mol;&c_z;&c_a;&c_m;&c_ib;&c_s;&c_t;</trd>
  <trdgrp lng="fr">
    <trd>parachite</trd>,
    <trd>paraschiste</trd>
  </trdgrp>
  <trd lng="pl">balsamista</trd>
  <trd lng="pt">parachita</trd>
  <trd lng="ro">&icirc;mb&abreve;ls&abreve;m&abreve;torul</trd>
</drv>
</art>

