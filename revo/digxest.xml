<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: digxest.xml,v 1.9 2024/04/03 07:21:38 revo Exp $">
<kap>
  <rad>di&gcirc;est</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="digxest.0o">
  <kap><tld/>o</kap>
  <snc mrk="digxest.0o.BELE">
    <dif>
      <ref tip="dif" cel="epitom.0o.BELE">Epitomo</ref>:
      <ekz>
        en Usono kaj anka&ubreve; alilande aperas <tld/>oj, t.e.
        densigitaj verkoj, alivorte, grandaj arta&jcirc;oj en po&scirc;formato:
        <fnt>
          <aut>Poul Thorsen</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr24/digxestoju.html">Di&gcirc;estoj</url></vrk>,
          <lok>Nica Literatura Revuo, 1957-03 &gcirc;is 04, numero 2/4,
          p. 138a-139a</lok>
        </fnt>;
      </ekz>
      <ekz>
        Esperanto-Buroo <klr>[&#8230;]</klr> eldonis <ctl>Sovetan Pedagogian
        Revuon</ctl>, aliajn materialojn en Esperanto kaj ruslingvan
        <tld/>on de la Esperanto-gazetaro
        <fnt>
          <aut>A. Kor&jcirc;enkov</aut>,
          <vrk>Historio de Esperanto</vrk>,
          <lok>Kaliningrado: Sezonoj, 2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="antolo.0o">antologio</ref>,
      <ref cel="kompen.0o">kompendio</ref>,
      <ref cel="kompil.0ajxo">kompila&jcirc;o</ref>,
      <ref cel="resum.0o">resumo</ref>,
      <ref cel="ekstra.0o.TIP">ekstrakto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_j;&c_d;&c_zh;&c_e;&c_s;&c_t;</trd>,
    <trd>&c_r;&c_e;&c_f;&c_e;&c_r;&c_a;&c_t;</trd>
  </trdgrp>
  <trd lng="de">Digest</trd>
  <trdgrp lng="fr">
    <trd>condens&eacute;<klr> (ouvrage)</klr></trd>,
    <trd>digest</trd>,
    <trd>&eacute;pitom&eacute;</trd>
  </trdgrp>
  <trd lng="he">&#x5E2;&#x5DB;&#x5BC;&#x5D5;&#x5BC;&#x5DC;</trd>
  <trdgrp lng="pl">
    <trd>bryk</trd>,
    <trd>streszczenie</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>ep&iacute;tome</trd>,
    <trd>resumo</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_a;&c_j;&c_d;&c_zh;&c_je;&c_s;&c_t;</trd>
  <trd lng="uk">&c_d;&c_a;&c_j;&c_d;&c_zh;&c_je;&c_s;&c_t;</trd>
</drv>
</art>

