<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: diktat.xml,v 1.29 2023/11/04 14:29:02 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>diktator</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="diktat.0o">
  <kap><tld/>o</kap>
  <snc mrk="diktat.0o.POL">
  <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="regn.0estro.POL">Regnestro</ref>
      la&ubreve;le&gcirc;e ricevinta absolutan povon:
      <ekz>
        en Romio, en tempo de dan&gcirc;ero, la senato povis nomumi
        <tld/>on por sesmonata regado;
      </ekz>
      <ekz>
        komenci&gcirc;is longa kaj severa re&gcirc;imo de la <tld/>o
        Franko
        <fnt>
        <aut>Urs'</aut>,
        <vrk>Mia Ujo en la Reto</vrk>,
        <lok>2006-04-09</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        hejme &scirc;i estas <tld/>o <klr>(tre ordonema persono)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="auxtok.0o">a&ubreve;tokrato</ref>,
      <ref cel="despot.0o">despoto</ref>,
      <ref cel="tiran.0o">tirano</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_k;&c_t;&c_a;&c_t;&c_a;&c_r;</trd>
  <trd lng="ca">dictador</trd>
  <trd lng="cs">dikt&#xE1;tor</trd>
  <trdgrp lng="de">
    <trd>Diktator</trd>,
    <trd>Gewaltherrscher</trd>
  </trdgrp>
  <trd lng="en">dictator</trd>
  <trd lng="es">dictador</trd>
  <trd lng="fr">dictateur</trd>
  <trd lng="he">&#x5D3;&#x5B4;&#x5D9;&#x5E7;&#x5D8;&#x5B8;&#x5D8;&#x5D5;&#x5B9;&#x5E8;</trd>
  <trd lng="hu">dikt&aacute;tor</trd>
  <trd lng="ja">&#x72EC;&#x88C1;&#x8005;  <pr>&#x3069;&#x304F;&#x3055;&#x3044;&#x3057;&#x3083;</pr></trd>
  <trd lng="pl">dyktator</trd>
  <trd lng="pt">ditador</trd>
  <trd lng="ru">&c_d;&c_i;&c_k;&c_t;&c_a;&c_t;&c_o;&c_r;</trd>
  <trd lng="sk">dikt&#xE1;tor</trd>
  <trd lng="sv">diktator</trd>
  <trd lng="uk">&#x434;&#x438;&#x43A;&#x442;&#x430;&#x442;&#x43E;&#x440;</trd>
  <trdgrp lng="zh">
    <trd>&#x4F7F;&#x4EBA;&#x542C;&#x5199;&#x8005; <pr>sh&#x1D0;r&#xE9;nt&#x12B;ngxi&#x11B;zh&#x11B;</pr></trd>,
    <trd>&#x72EC;&#x88C1;&#x653F;&#x6743;&#x6267;&#x653F;&#x8005; <pr>d&#xFA;c&#xE1;izh&#xE8;ngqu&#xE1;nzh&#xED;zh&#xE8;ngzh&#x11B;</pr></trd>,
    <trd>&#x53E3;&#x6388;&#x8005; <pr>k&#x1D2;ush&#xF2;uzh&#x11B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="diktat.0a">
  <kap><tld/>a</kap>
  <snc mrk="diktat.0a.POL">
    <dif>
      Rilata al <tld/>o, al lia regado; inda je <tld/>o:
      <ekz>
        la eksterlanda politiko de Usono apogas nuntempe alian <tld/>an
        re&gcirc;imon
        <fnt>
        <aut>F&aacute;tima</aut>,
        <vrk>Mia haveno en Esperantio</vrk>,
        <lok>2005-09-19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_k;&c_t;&c_a;&c_t;&c_a;&c_r;&c_s;&c_k;&c_ib;</trd>
  <trd lng="ca">dictatorial</trd>
  <trd lng="de">diktatorisch</trd>
  <trd lng="en">dictatorial</trd>
  <trd lng="es">dictatorial</trd>
  <trd lng="fr">dictatorial</trd>
  <trd lng="he">&#x5E8;&#x5D5;&#x5D3;&#x5E0;&#x5D9;</trd>
  <trdgrp lng="hu">
    <trd>dikt&aacute;tori</trd>,
    <trd>diktat&oacute;rikus</trd>
  </trdgrp>
  <trd lng="ja">&#x72EC;&#x88C1;&#x8005;&#x306E;  <pr>&#x3069;&#x304F;&#x3055;&#x3044;&#x3057;&#x3083;&#x306E;</pr></trd>
  <trd lng="pl">dyktatorski</trd>
  <trd lng="ru">&c_d;&c_i;&c_k;&c_t;&c_a;&c_t;&c_o;&c_r;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="uk">&#x434;&#x438;&#x43A;&#x442;&#x430;&#x442;&#x43E;&#x440;&#x441;&#x44C;&#x43A;&#x438;&#x439;</trd>
  <trd lng="zh">&#x98DB;&#x63DA;&#x8DCB;&#x6248; <pr>f&#x113;iy&#xE1;ngb&#xE1;h&#xF9;</pr></trd>
</drv>

<drv mrk="diktat.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="diktat.0eco.iu">
    <dif>
      Eco de <tld/>o.
    </dif>
    <trd lng="be">&c_d;&c_y;&c_k;&c_t;&c_a;&c_t;&c_a;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>
    <trd lng="hu">dikt&aacute;tors&aacute;g</trd>    
    <trd lng="ru">&c_d;&c_i;&c_k;&c_t;&c_a;&c_t;&c_o;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>
  </snc>
  <snc mrk="diktat.0eco.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Regado de <tld/>o;
      <ref tip="sin" cel="diktat1.0o.POL">diktaturo</ref>:
      <ekz>
        <klr>[en Romio]</klr> plej diversaj regadformoj estis
        elprovataj: re&gcirc;eco, respubliko, <tld/>eco, imperiestreco...
        <fnt>
        <bib>Lanti</bib>,
        <lok>&Ccirc;e la lando de la nigra&ccirc;emizuloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        en &Ccirc;inio <klr>...</klr> sub la <tld/>eco de la
        &Ccirc;ina Komunista Partio ne ekzistas penslibereco
        <fnt>
        <aut>Kurisu Kei</aut>,
        <vrk>Penslibereco en &Ccirc;inio</vrk>,
        <bib>Monato</bib>,
        <lok>2002:2, p. 6a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_d;&c_y;&c_k;&c_t;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
    <trd lng="ca">dictadura</trd>
    <trd lng="es">dictadura</trd>
    <trd lng="hu">diktat&uacute;ra</trd>    
    <trd lng="pl">dyktatura</trd>
    <trd lng="ru">&c_d;&c_i;&c_k;&c_t;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
  </snc>
  <trd lng="bo">&#xF66;&#xFB2;&#xF72;&#xF51;&#xF0B;&#xF51;&#xF56;&#xF62;&#xF0B;&#xF66;&#xF92;&#xF7A;&#xF62;&#xF0B;&#xF60;&#xF5B;&#xF72;&#xF53;&#xF0B;</trd>
  <trd lng="cs">diktatura</trd>
  <trd lng="en">dictatorship</trd>
  <trd lng="fr">dictature</trd>
  <trdgrp lng="ja">
    <trd>&#x72EC;&#x88C1;  <pr>&#x3069;&#x304F;&#x3055;&#x3044;</pr></trd>,
    <trd>&#x7D76;&#x5BFE;&#x7684;&#x6A29;&#x529B;  <pr>&#x305C;&#x3063;&#x305F;&#x3044;&#x3066;&#x304D;&#x3051;&#x3093;&#x308A;&#x3087;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="sk">diktat&#xFA;ra</trd>
  <trd lng="uk">&#x434;&#x438;&#x43A;&#x442;&#x430;&#x442;&#x443;&#x440;&#x430;</trd>
</drv>

<drv mrk="diktat.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="diktat.0ema.KOMUNE">
    <dif>
      Ema trudi siajn decidojn:
      <ekz>
        <tld/>ema gvidanto <klr>...</klr> kontenti&gcirc;as pri la 
        pasivismo de la grupanoj
        <fnt>
        <aut>B. Golden</aut>,
        <vrk>Malgvidantoj de pasivistoj</vrk>,
        <lok><url ref="http://www.eventoj.hu/arkivo/eve-053.htm">
        Eventoj, 1994-05, numero 53a</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_k;&c_t;&c_a;&c_t;&c_a;&c_r;&c_s;&c_k;&c_ib;</trd>
  <trdgrp lng="ca">
    <trd>autoritari</trd>,
    <trd>dominant</trd>
  </trdgrp>
  <trd lng="en">overbearing</trd>
  <trd lng="es">dictatorial</trd>
  <trd lng="fr">dictatorial<klr tip="ind"> (fig.)</klr></trd>
  <trd lng="he">&#x5E8;&#x5D5;&#x5B9;&#x5D3;&#x5B8;&#x5E0;&#x5B4;&#x5D9;</trd>
  <trdgrp lng="hu">
    <trd>dikt&aacute;tori hajlam&uacute;</trd>,
    <trd>diktat&oacute;rikus</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x72EC;&#x88C1;&#x7684;&#x306A;  <pr>&#x3069;&#x304F;&#x3055;&#x3044;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x6A2A;&#x66B4;&#x306A;  <pr>&#x304A;&#x3046;&#x307C;&#x3046;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dyktatorski</trd>,
    <trd>apodyktyczny</trd>,
    <trd>despotyczny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_i;&c_k;&c_t;&c_a;&c_t;&c_o;&c_r;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_l;&c_a;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_d;&c_je;&c_s;&c_p;&c_o;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dikt&#xE1;torsk&#xFD;</trd>,
    <trd>panova&#x10D;n&#xFD;</trd>
  </trdgrp>
</drv>
<adm>
  Indas trovi a&ubreve;toritatajn ekz-ojn de la figursenca uzo
  kaj anka&ubreve; ekz-ojn, montrantajn sencnuancojn inter
  ~a, ~eca, ~ema, ~eska... &Scirc;ajnas, ke ~ema rilatas al
  figura senco de ~o.
  <aut>MB</aut>
</adm>
</art>
