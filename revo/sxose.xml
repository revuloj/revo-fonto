<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxose.xml,v 1.71 2021/06/18 04:36:01 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>&scirc;ose</rad>/o
</kap>

<drv mrk="sxose.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxose.0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      <ref tip="super" cel="voj.0o.TRA">Vojo</ref>, precipe eksterurba kun malmola surfaco,
      speciale konstruita por veturiloj:
      <ekz>
        la vojo de tiu domo al la <ind>publika <tld/>o</ind> estas
        tre
        malbonstata
        <fnt><aut>Donald Broadribb</aut>,
          <vrk><url ref="&Monato;/rec/surk.html">Inter la dek plej bonaj romanoj</url></vrk>,
          <bib>Monato</bib>, <lok>n-ro 11/2000</lok>.
        </fnt>;
        <trd lng="ca">via p&uacute;blica</trd>
      </ekz><ekz>
        de tie iris du <tld/>oj al la havenoj de Ru&gcirc;a maro: <nom>Koseir</nom>
        kaj <nom>Berenice</nom>, kaj vojo al la porfiraj montoj <klr>[&#x2026;]</klr>
	<fnt><bib>Far3</bib>, <lok>&ccirc;ap. 9a.</lok></fnt>;
      </ekz><ekz>
        se vi rigardos mapon de Irlando, vi trovos &gcirc;in
        <klr>(la vila&gcirc;on <nom>Geashill</nom>)</klr> sur la <tld/>o R420 inter
        la iamaj hugenotaj urboj <nom>Portarlington</nom> kaj <nom>Tullamore</nom>
        <fnt>
          <aut>Albisturo KVINKE</aut>: <vrk>Tenebro</vrk>.
          <bib>Monato</bib>
        </fnt>;
      </ekz><ekz>
        en estonteco <klr>(...)</klr> vojoj, klare, |
        &ccirc;e ni &scirc;an&gcirc;i&gcirc;os senkompare: |
        <tld/>oj &ccirc;ie tra Rusland' |
        plekti&gcirc;os en abunda kvant'
        <fnt>
          <aut>A. Pu&scirc;kin, tr. V. Melnikov</aut>:
          <vrk>E&ubreve;geno Onegin</vrk>.
          <lok>Kaliningrado: Sezonoj, 2005. 7:33, p. 153</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo><uzo tip="fak">TEL</uzo>
        por tiuj aliaj, kiuj ankora&ubreve; malmulton scias pri la interreta 
        <ind>informa <tld/>o</ind>, Monato planas publikigi kurson
        en la venonta jarkolekto.
        <trdgrp lng="be">
          <trd>&c_k;&c_a;&c_n;&c_a;&c_l;</trd>,
          <trd>&c_m;&c_a;&c_g;&c_ib;&c_s;&c_t;&c_r;&c_a;&c_l;&c_mol; <klr>(&c_a;&c_b;&c_m;&c_je;&c_n;&c_u; &c_ib;&c_n;&c_f;&c_a;&c_r;&c_m;&c_a;&c_c;&c_y;&c_ja;&c_j;)</klr></trd>
        </trdgrp>
        <trd lng="ca">canal de transmissi&oacute;, d'informaci&oacute;</trd>
        <trd lng="de">Datenautobahn</trd>
        <trd lng="en">information superhighway</trd>
        <trd lng="fr">autoroute de l'information</trd>
        <trd lng="hu">inform&aacute;ci&oacute;s szupersztr&aacute;da</trd>
        <trd lng="it">autostrada informatica</trd>
        <trd lng="nl">informatiesnelweg</trd>
        <fnt><bib>Monato</bib>, <lok>n-ro 12/1997</lok>.
        </fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ale.0o.TRA">aleo<sncref/></ref>,
      <ref cel="avenu.0o">avenuo</ref>,
      <ref cel="strat.0o">strato</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_sh;&c_a;&c_sh;&c_a;</trd>,
      <trd>&c_g;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_n;&c_je;&c_c;</trd>,
      <trd>&c_b;&c_a;&c_l;&c_mol;&c_sh;&c_a;&c_k;</trd>
    </trdgrp>
    <trd lng="bg">&c_a;&c_v;&c_t;&c_o;&c_m;&c_a;&c_g;&c_i;&c_s;&c_t;&c_r;&c_a;&c_l;&c_a;</trd>
    <trdgrp lng="ca">
      <trd>carretera</trd>,
      <trd>via</trd>
    </trdgrp>
    <trd lng="de">Landstra&szlig;e</trd>
    <trd lng="en">highway</trd>
    <trdgrp lng="fr">
      <trd>chauss&eacute;e</trd>,
      <trd>route</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>m&udblac;&uacute;t</trd>,
      <trd>k&ouml;z&uacute;t</trd>,
      <trd>orsz&aacute;g&uacute;t</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>snelweg</trd>,
      <trd>autoweg</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_sh;&c_o;&c_s;&c_s;&c_je;</trd>,
      <trd>&c_m;&c_a;&c_g;&c_i;&c_s;&c_t;&c_r;&c_a;&c_l;&c_mol;</trd>,
      <trd>&c_a;&c_v;&c_t;&c_o;&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;</trd>
    </trdgrp>
    <trd lng="pt">rodovia</trd>
    <trdgrp lng="it">
      <trd>autostrada</trd>,
      <trd>superstrada</trd>, 
      <trd>statale<klr> (strada)</klr></trd>, 
      <trd>carreggiata</trd>
    </trdgrp>
  </snc>
  <snc mrk="sxose.0o.maltrotuaro">
    <uzo tip="stl">RAR</uzo>
    <ref tip="dif" cel="vetur.0ejo.sxoseo">veturejo</ref>
    <trd lng="ca">cal&ccedil;ada</trd>
    <trdgrp lng="nl">
      <trd>straatweg</trd>,
      <trd>steenweg</trd>
    </trdgrp>
  </snc>
  <trd lng="af">pad</trd>
  <trd lng="am">&#x1218;&#x1295;&#x1308;&#x12F5;</trd>
  <trd lng="ar">&#x637;&#x631;&#x64A;&#x642;</trd>
  <trd lng="az">yol</trd>
  <trd lng="bn">&#x9B0;&#x9BE;&#x9B8;&#x9CD;&#x9A4;&#x9BE;</trd>
  <trd lng="bo">&#xF63;&#xF58;&#xF0B;&#xF41;&#xF42;&#xF0B;</trd>
  <trd lng="bs">putu</trd>
  <trd lng="co">stradale</trd>
  <trd lng="da">vej</trd>
  <trdgrp lng="es">
    <trd>autov&iacute;a</trd>,
    <trd>carretera</trd>
  </trdgrp>
  <trd lng="et">tee</trd>
  <trd lng="eu">errepide</trd>
  <trd lng="fy">wei</trd>
  <trd lng="gl">estrada</trd>
  <trd lng="gu">&#xAAE;&#xABE;&#xAB0;&#xACD;&#xA97;</trd>
  <trd lng="hi">&#x938;&#x921;&#x93C;&#x915;</trd>
  <trd lng="hr">cesta</trd>
  <trd lng="hy">&#x573;&#x561;&#x576;&#x561;&#x57A;&#x561;&#x580;&#x570;&#x561;&#x575;&#x56B;&#x576;</trd>
  <trd lng="id"><ind>jalan</ind> raya</trd>
  <trd lng="is">vegum</trd>
  <trd lng="ja">&#x9053;&#x8DEF;</trd>
  <trd lng="jw">dalan</trd>
  <trd lng="ka">&#x10D2;&#x10D6;&#x10D8;&#x10E1;</trd>
  <trd lng="km">&#x1795;&#x17D2;&#x179B;&#x17BC;&#x179C;</trd>
  <trd lng="ko">&#xB3C4;&#xB85C;</trd>
  <trd lng="lo">&#xE96;&#xEB0;&#xEAB;&#xE99;&#xEBB;&#xE99;&#xEAB;&#xEBB;&#xE99;&#xE97;&#xEB2;&#xE87;</trd>
  <trd lng="ml">&#xD31;&#xD4B;&#xD21;&#xD4D;</trd>
  <trd lng="mr">&#x930;&#x938;&#x94D;&#x924;&#x93E;</trd>
  <trd lng="my">&#x101C;&#x1019;&#x103A;&#x1038;</trd>
  <trd lng="ne">&#x938;&#x921;&#x915;</trd>
  <trd lng="ny">msewu</trd>
  <trd lng="pa">&#xA38;&#xA5C;&#xA15;</trd>
  <trd lng="ps">&#x62F; &#x633;&#x693;&#x6A9;</trd>
  <trd lng="sd">&#x631;&#x633;&#x62A;&#x64A; &#x6FE;</trd>
  <trd lng="si">&#xDB8;&#xDCF;&#xDBB;&#xDCA;&#xD9C;</trd>
  <trd lng="sq">rrug&#xEB;</trd>
  <trd lng="ta">&#xB9A;&#xBBE;&#xBB2;&#xBC8;</trd>
  <trd lng="te">&#xC30;&#xC4B;&#xC21;&#xC4D;&#xC21;&#xC41;</trd>
  <trd lng="th">&#xE16;&#xE19;&#xE19;</trd>
  <trd lng="tl">kalsada</trd>
  <trd lng="ur">&#x633;&#x691;&#x6A9;</trd>
  <trd lng="vi">&#x111;&#x1B0;&#x1EDD;ng</trd>
  <trd lng="yi">&#x5D5;&#x5D5;&#x5E2;&#x5D2;</trd>
  <trd lng="zu">umgwaqo</trd>
</drv>

<drv mrk="sxose.auxto0o">
  <kap>
    a&ubreve;to<tld/>o
  </kap>
  <uzo tip="fak">TRA</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="voj.0o.TRA">Vojo</ref> plurkoridora, konstruita por rapida veturado de a&ubreve;toj ekskluzive:
      <ekz>
        sudan Germanion kaj nordan Italion interligas la a&ubreve;to<tld/>o &leftquot;<nom>Brenner</nom>&rightquot;, kiu
        kondukas tra la a&ubreve;stria federacia regiono Tirolo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007102.html">Protestas naj-baroj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en multaj aliaj urbegoj la konstante kreskanta trafiko estigis gravajn
        transportproblemojn, kiujn la konstruo de lar&gcirc;aj a&ubreve;to<tld/>oj kaj novaj
        pontoj ne sukcesis solvi
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="https://www.monato.be/2006/008699.php">Kara antikva malpura Dublino</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        por &gcirc;eni la kunvenon, homamasoj blokis a&ubreve;to<tld/>ojn kaj &ccirc;iujn pontojn super
        Rodano
        <fnt>
          <bib>Monato</bib>
          <aut>Claude Piron</aut>
          <vrk><url ref="https://www.monato.be/2003/008141.html">Francio invitas, Svislando suferas</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Autobahn</trd>
</drv>


