<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kondiment.xml,v 1.21 2023/12/03 09:58:55 revo Exp $">
<kap>
  <rad>kondiment</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kondiment.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="kondiment.0o.spicajxo">
    <dif>
      <ref tip="dif" cel="spic.0ajxo">Spica&jcirc;o</ref>:
      <ekz>
        <ref tip="sub" cel="vinagr.0o">vinagro</ref> <klr>[&#x2026;]</klr> estas ofte uzata en kuirarto, &ccirc;efe por 
        vinagrosa&ubreve;coj kaj peklado, oni anka&ubreve; povas uzi &gcirc;in kiel <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=41165">Vinagro</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref tip="sub" cel="miso.0o">misoo</ref> estas japana <tld/>o, kiu konsistas plejparte el
        fermentataj sojfaboj kun variaj partoj de rizo, salo a&ubreve; hordeo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=83886">Misoo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref tip="sub" cel="cxatni.0o">&ccirc;atnio</ref> estas spicita konfita&jcirc;o a&ubreve; <tld/>o, precipe el mangoj,
        man&gcirc;ata kutime kun viando
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=125925">&Ccirc;atnio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la vila&gcirc;o starigis modernan industrian komplekson de 26 fabrikoj
        koncernaj al man&gcirc;a&jcirc;oj, <tld/>oj, farmacio, trinka&jcirc;oj, biero,
        presado, pakado ktp
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="&Monato;/2011/010245.php">Floras
          enklavo komunista</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>W&uuml;rze</trd>,
    <trd>Gew&uuml;rz</trd>
  </trdgrp>
  <trd lng="en">condiment</trd>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_p;&c_r;&c_a;&c_v;&c_a;</trd>,
    <trd>&c_s;&c_p;&c_e;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
  <trd lng="cs">pochutina</trd>
  <trdgrp lng="fa">
    <trd>&f_ch;&a_A;&a_sh;&a_n;&f_y;</trd>,
    <trd>&a_A;&a_d;&a_w;&f_y;&a_h;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>condiment</trd>,
    <trd>assaisonnement</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8ABF;&#x5473;&#x6599;  <pr>&#x3061;&#x3087;&#x3046;&#x307F;&#x308A;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x85AC;&#x5473;  <pr>&#x3084;&#x304F;&#x307F;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>specerij</trd>,
    <trd>kruiderij</trd>
  </trdgrp>
  <trd lng="pl">przyprawa</trd>
  <trdgrp lng="pt">
    <trd>tempero</trd>,
    <trd>condimento</trd>
  </trdgrp>
  <trd lng="ro">condimente</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_p;&c_r;&c_a;&c_v;&c_a;</trd>,
    <trd>&c_s;&c_p;&c_je;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">pochutina</trd>
  <trdgrp lng="zh">
    <trd>&#x8ABF;&#x6599; <pr>ti&#xE1;oli&#xE0;o</pr></trd>,
    <trd>&#x8ABF;&#x5473;&#x6599; <pr>ti&#xE1;ow&#xE8;ili&#xE0;o</pr></trd>,
    <trd>&#x9999;&#x6599; <pr>xi&#x101;ngli&#xE0;o</pr></trd>,
    <trd>&#x8ABF;&#x5473;&#x54C1; <pr>ti&#xE1;ow&#xE8;ip&#x1D0;n</pr></trd>,
    <trd>&#x4F5C;&#x6599; <pr>zu&#xF3;liao</pr></trd>,
    <trd>&#x4F50;&#x6599; <pr>zu&#x1D2;li&#xE0;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kondiment.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kondiment.0i.KUI">
    <dif>
      Pligustigi per <tld/>o:
      <ekz>
        la bovida&jcirc;on <klr>[&#x2026;]</klr> vi povus iomete gustumi, se &gcirc;i ne
        estus rostita kaj <tld/>ita
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 47a, p.
        652a</lok></fnt>;
      </ekz>
      <ekz>
        torteloj <klr>[&#x2026;]</klr> <tld/>itaj per tomata sa&ubreve;co
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2012/010845.php">Saturnia: Vintra
          magio</url></vrk>
        </fnt>.
      </ekz>      
    </dif>
    <ref tip="vid" cel="spic.0i.mangxon">spici<sncref/></ref>
  </snc>
  <trd lng="de">w&uuml;rzen</trd>
  <trd lng="en">to flavor with condiment</trd>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_p;&c_r;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_r;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol; <klr tip="amb">(&c_je;&c_zh;&c_u;)</klr></trd>
  </trdgrp>
  <trd lng="cs">ochutit</trd>
  <trdgrp lng="fa">
    <trd>&f_ch;&a_A;&a_sh;&a_n;&f_y; &a_z;&a_d;&a_n;<klr> (&a_b;&a_h;)</klr></trd>,
    <trd>&a_A;&a_d;&a_w;&f_y;&a_h; &a_z;&a_d;&a_n;<klr> (&a_b;&a_h;)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>condimenter</trd>,
    <trd>assaisonner</trd>
  </trdgrp>
  <trd lng="ja">&#x5473;&#x4ED8;&#x3051;&#x3059;&#x308B;  <pr>&#x3042;&#x3058;&#x3064;&#x3051;&#x3059;&#x308B;</pr></trd>
  <trd lng="nl">kruiden</trd>
  <trd lng="pl">przyprawia&cacute;</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_p;&c_r;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>
  <trd lng="sk">ochuti&tcaron;</trd>
  <trdgrp lng="zh">
    <trd>&#x8ABF;&#x5473; <pr>ti&#xE1;ow&#xE8;i</pr></trd>,
    <trd>&#x9999;&#x5473; <pr>xi&#x101;ngw&#xE8;i</pr></trd>
  </trdgrp>
</drv>

<adm>
  (1) Cxu salo estas kondimento?
  <aut>MB</aut>
</adm>
<adm>
  Sxajnas al mi, ke kondimento estas plantdevena; ekskludas do salon.
  <aut>PP</aut>
</adm>

</art>

