<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>no</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="no.0o">
  <kap><tld/>o</kap>
  <snc mrk="no.0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Japana klasika <ref tip="super" cel="teatr.0o.belarto">teatro</ref>
      kun maskoj, &ccirc;anto, &hcirc;oro kaj muzikakompano, malmultaj roluloj, sobra
      dancado:
      <ekz>
        teceremonio kaj <tld/>o amba&ubreve; originis el &Ccirc;inio
        <fnt>
        <aut>I. Degu&ccirc;i, trad. N. Esumi</aut>,
        <vrk><url
        ref="http://www.oomoto.or.jp/Esperanto/esKuljap/esenco/es04.html">La
        esenco de la japana kulturo</url></vrk>,
        <bib>Oomoto</bib>,
        <lok>1987, numero 426a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">No-Theater</trd>
  <trd lng="fr">n&ocirc;</trd>
  <trd lng="hu">n&oacute;</trd>
  <trd lng="nl">no-theater</trd>
  <trd lng="pt">n&ocirc;</trd>
</drv>
</art>
