<art mrk="$Id: no.xml,v 1.15 2024/08/20 09:29:39 revo Exp $">
<kap>
  <rad>no</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="no.0o">
  <kap><tld/>o</kap>
  <snc mrk="no.0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Japana klasika <ref tip="super" cel="teatr.0o.belarto">teatro</ref>
      kun maskoj, &ccirc;anto, &hcirc;oro kaj muzikakompano, malmultaj roluloj, sobra
      dancado:
      <ekz>
        teceremonio kaj <tld/>o amba&ubreve; originis el &Ccirc;inio
        <fnt>
        <aut>I. Degu&ccirc;i, trad. N. Esumi</aut>,
        <vrk><url ref="http://www.oomoto.or.jp/Esperanto/esKuljap/esenco/es04.html">La
        esenco de la japana kulturo</url></vrk>,
        <bib>Oomoto</bib>,
        <lok>1987, numero 426a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_o; <klr>(&c_ja;&c_p;&c_o;&c_n;&c_s;&c_k;&c_ib; &c_zh;&c_a;&c_n;&c_r;)</klr></trd>
  <trdgrp lng="cs">
    <trd>no</trd>,
    <trd>no-hra</trd>
  </trdgrp>
  <trd lng="de">No-Theater</trd>
  <trd lng="fr">n&ocirc;</trd>
  <trd lng="hu">n&oacute;</trd>
  <trd lng="ja">&#x80FD;  <pr>&#x306E;&#12358;</pr></trd>
  <trd lng="nl">no-theater</trd>
  <trd lng="pl">N&omacron;</trd>
  <trd lng="pt">n&ocirc;</trd>
  <trd lng="ro">N&omacron;</trd>
  <trd lng="sk">no</trd>
  <trd lng="uk">&#x43D;&#x43E; (&#x43B;&c_ib;&c_r;&c_i;&c_ch;&#x43D;&c_a; &c_d;&c_r;&c_a;&#x43C;&c_a;)</trd>
  <trdgrp lng="zh">
    <trd>&#x80FD;&#21095; <pr>n&#xE9;ngj&#xF9;</pr></trd>,
    <trd>&#x80FD;&#21127; <pr>n&#xE9;ngj&#xF9;</pr></trd>
  </trdgrp>
</drv>
</art>
