<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: korola.xml,v 1.19 2023/09/01 15:46:44 revo Exp $">
<kap>
  <rad>korolari</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="korola.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 6</lok></fnt>
  <snc>
    <dif>
      Facile pruvebla <ref tip="super" cel="konsek1.0o">konsekvenco</ref>
      de <ref cel="teorem.0o">teoremo</ref>:
      <ekz>
        la konserva le&gcirc;o de movokvanto, kiun Ne&ubreve;tono derivis kiel <tld/>o al lia dua kaj
        tria le&gcirc;oj estis la unua konserva le&gcirc;o malkovrita
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=49576">Le&gcirc;oj de Newton pri movo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li citas <nom>George Steiner</nom>: &leftquot;tutmonda kreola lingvo, bazita sur la usona angla,
        estas detrua perspektivo&#x201D;, la hodia&ubreve;a tutmondigo, kun siaj lingvaj <tld/>oj,
        kunportas aliajn gravajn konsekvencojn <klr>[&#x2026;]</klr>
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2004/008394.html">La angla, &ccirc;u troja &ccirc;evalo?</url></vrk>
              <bib>Monato</bib>
              <lok>2004/10, p. 20</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="eviden.0a">evidenta</ref>,
       <ref cel="don.al0o.kromajho">aldono</ref>,
       <ref cel="donac.0o">donaco</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_k;&c_a;&c_r;&c_a;&c_l;&c_ja;&c_r;&c_y;&c_j;</trd>,
      <trd>&c_v;&c_y;&c_s;&c_n;&c_o;&c_v;&c_a;</trd>
    </trdgrp>
    <trd lng="de">Korollar</trd>
    <trd lng="en">corollary</trd>
    <trdgrp lng="fa">
      <trd>&a_n;&a_t;&f_y;&a_j;&a_h;</trd>,
      <trd>&a_f;&a_r;&a_ayn;</trd>
    </trdgrp>
    <trd lng="fr">corollaire</trd>
    <trdgrp lng="hu">
      <trd>koroll&aacute;rium</trd>,
      <trd>egyenes <ind>k&ouml;vetkezm&eacute;ny</ind></trd>,      
     <trd>folyom&aacute;ny</trd>     
    </trdgrp>
    <trd lng="nl">corollarium</trd>
    <trdgrp lng="pl">
      <trd>korolarium</trd>,
      <trd>twierdzenie dowiedzione</trd>
    </trdgrp> 
    <trd lng="pt">corol&aacute;rio</trd>
    <trd lng="ro">teorema dovedit</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_r;&c_o;&c_l;&c_l;&c_a;&c_r;&c_i;&c_j;</trd>,
      <trd>&c_s;&c_l;&c_je;&c_d;&c_s;&c_t;&c_v;&c_i;&c_je;<klr tip="ind">
      (&c_t;&c_je;&c_o;&c_r;&c_je;&c_m;&c_y;)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="ja">&#x7CFB;  [<ind>&#x3051;&#x3044;</ind>]</trd>
</drv>
</art>
