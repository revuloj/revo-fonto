<vortaro>
<art mrk="$Id$">
<kap>
  <rad>korolari</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="korola.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 6</lok></fnt>
  <snc>
    <dif>
      Facile pruvebla konsekvenco de <ref cel="teorem.0o">teoremo</ref>.
    </dif>
    <trd lng="de">Korollar</trd>
    <trd lng="en">corollary</trd>
    <trd lng="fr">corollaire</trd>
    <trdgrp lng="hu">
      <trd>koroll&aacute;rium</trd>,
      <trd>egyenes <ind>k&ouml;vetkezm&eacute;ny</ind></trd>,      
     <trd>folyom&aacute;ny</trd>     
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_r;&c_o;&c_l;&c_l;&c_a;&c_r;&c_i;&c_j;</trd>,
      <trd>&c_s;&c_l;&c_je;&c_d;&c_s;&c_t;&c_v;&c_i;&c_je;<klr tip="ind"> (&c_t;&c_je;&c_o;&c_r;&c_je;&c_m;&c_y;)</klr></trd>
    </trdgrp>
  </snc>
</drv>
</art>
