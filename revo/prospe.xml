<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>prospekt</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="prospe.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Presita reklamo anoncanta aperon de verko, malfermon de establejo kaj
      disdonata por altiri klientojn.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_s;&c_p;&c_e;&c_k;&c_t; <klr>(&c_r;&c_e;&c_k;&c_l;&c_ja;&c_m;&c_n;&c_y;)</klr></trd>
  <trdgrp lng="de">
    <trd>Prospekt</trd>,
    <trd>Werbeschrift</trd>
  </trdgrp>
  <trd lng="hu">prospektus</trd>
  <trd lng="nl">prospectus</trd>
  <trdgrp lng="pt">
    <trd>prospecto</trd>,
    <trd>avulso</trd>,
    <trd>folha <klr tip="ind">volante</klr></trd>,
    <trd>folheto</trd>,
    <trd>volante</trd>,
    <trd>filipeta</trd>    
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_k;&c_l;&c_a;&c_m;&c_n;&c_y;&c_j; <ind>&c_p;&c_r;&c_o;&c_s;&c_p;&c_je;&c_k;&c_t;</ind></trd>
</drv>
</art>
