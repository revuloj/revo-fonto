<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>prospekt</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="prospe.0o">
  <kap><tld/>o</kap>
  <snc mrk="prospe.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Reklama <ref tip="vid" cel="foli.flug0o.TIP">flugfolio</ref>
      anoncanta aperon de verko, malfermon de establejo
      kaj disdonata por altiri klientojn:
      <ekz>
        la spektantoj foriris lasinte la <tld/>ojn
        sur la malplenaj se&gcirc;oj
        <fnt>
          <aut>B. Ragnarsson</aut>,
          <vrk>la&ubreve; neplanitaj padoj</vrk>,
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_s;&c_p;&c_e;&c_k;&c_t; <klr>(&c_r;&c_e;&c_k;&c_l;&c_ja;&c_m;&c_n;&c_y;)</klr></trd>
  <trd lng="bg">&c_p;&c_r;&c_o;&c_s;&c_p;&c_je;&c_k;&c_t;</trd>
  <trdgrp lng="ca">
    <trd>prospecte</trd>,
    <trd>reclam</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Prospekt</trd>,
    <trd>Werbeschrift</trd>
  </trdgrp>
  <trd lng="en">prospectus</trd>
  <trd lng="fr">prospectus</trd>
  <trd lng="hu">prospektus</trd>
  <trd lng="nl">prospectus</trd>
  <trdgrp lng="pt">
    <trd>prospecto</trd>,
    <trd>avulso</trd>,
    <trd>folha <klr tip="ind">volante</klr></trd>,
    <trd>folheto</trd>,
    <trd>volante</trd>,
    <trd>filipeta</trd>    
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_k;&c_l;&c_a;&c_m;&c_n;&c_y;&c_j; <ind>&c_p;&c_r;&c_o;&c_s;&c_p;&c_je;&c_k;&c_t;</ind></trd>
  <trdgrp lng="sk">
    <trd>propagačné tlačené oznámenie</trd>,
    <trd>prospekt</trd>
  </trdgrp>
</drv>
</art>
