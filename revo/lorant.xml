<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>lorant</rad>/acoj
</kap>
<drv mrk="lorant.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Loranthaceae</trd>), verdaj duonparazitoj,
      plej ofte kreskantaj en la bran&ccirc;aro de arboj.
    </dif>
  </snc>
  <trd lng="de">Mistelgew&auml;chse</trd>
  <trd lng="en">mistletoe</trd>
</drv>
</art>
