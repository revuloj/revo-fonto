<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>sandal</rad>/o
</kap>
<drv mrk="sandal.0o">
  <kap><tld/>o</kap>
  <snc mrk="sandal.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="sxu.0o.TEKS">&Scirc;uo</ref> konsistanta nur
      el plandumo ligita per &scirc;nuroj a&ubreve; rimenetoj al la piedo.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_n;&c_d;&c_a;&c_l;&c_ja;</trd>
  <trd lng="ca">sand&agrave;lia</trd>
  <trd lng="de">Sandale</trd>
  <trd lng="en">sandal</trd>
  <trdgrp lng="es">
    <trd>sandalia</trd>,
    <trd>huarache<klr tip="amb"> (Meksiko)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>nu-pieds</trd>,
    <trd>sandale</trd>
  </trdgrp>
  <trd lng="hu">szand&aacute;l</trd>
  <trd lng="nl">sandaal</trd>
  <trd lng="ru">&c_s;&c_a;&c_n;&c_d;&c_a;&c_l;&c_i;&c_ja;</trd>
</drv>
</art>
