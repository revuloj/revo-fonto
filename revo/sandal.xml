<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sandal.xml,v 1.26 2023/09/03 12:09:44 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>sandal</rad>/o
</kap>
<drv mrk="sandal.0o">
  <kap><tld/>o</kap>
  <snc mrk="sandal.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="lst" lst="voko:vesta&jcirc;oj" cel="sxu.0o.TEKS">&Scirc;uo</ref> konsistanta nur
      el plandumo ligita per &scirc;nuroj a&ubreve; rimenetoj al la piedo:
      <ekz>
        ili iru piedvestitaj per <tld/>oj, kaj ne surmetu du tunikojn
        <fnt>
          <bib>NT</bib>
          <lok>Marko 6:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel belaj estas viaj pa&scirc;oj en <tld/>oj, ho filino de princo
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 7:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        la an&gcirc;elo diris al li: zonu vin, kaj alligu viajn <tld/>ojn
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 12:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi metos sur iliajn nukojn mian piedon en fera <tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        krom plekti pajlajn <tld/>ojn a&ubreve; fliki vestojn la diligentaj soldatoj utiligas
        &ccirc;iun ripoz-horon por lernado
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="&Monato;/2015/011403.php">Misiisto, mar&scirc;ado kaj detenanta mano</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_n;&c_d;&c_a;&c_l;&c_ja;</trd>
  <trd lng="bg">&c_s;&c_a;&c_n;&c_d;&c_a;&c_l;</trd>
  <trd lng="ca">sand&agrave;lia</trd>
  <trd lng="cs">sand&aacute;l</trd>
  <trd lng="de">Sandale</trd>
  <trd lng="en">sandal</trd>
  <trdgrp lng="es">
    <trd>sandalia</trd>,
    <trd>huarache<klr tip="amb"> (Meksiko)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>nu-pieds</trd>,
    <trd>sandale</trd>
  </trdgrp>
  <trd lng="hu">szand&aacute;l</trd>
  <trd lng="id">sandalo</trd>
  <trd lng="it">sandalo</trd>
  <trdgrp lng="ja">
    <trd>&#x30B5;&#x30F3;&#x30C0;&#x30EB;</trd>,
    <trd>&#x3052;&#x305F;</trd>,
    <trd>&#x305E;&#x3046;&#x308A;</trd>
  </trdgrp>
  <trd lng="nl">sandaal</trd>
  <trd lng="pl">sanda&lstroke;</trd>
  <trdgrp lng="pt">
   <trd>sand&aacute;lia</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_a;&c_n;&c_d;&c_a;&c_l;&c_i;&c_ja;</trd>
  <trd lng="sk">sand&aacute;l</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_a;&#x43D;&c_d;&c_a;&#x43B;&#x44F;</trd>,
    <trd>&c_s;&c_a;&#x43D;&c_d;&c_a;&#x43B;&c_ib;&#x44F;</trd>
  </trdgrp>
</drv>

</art>
