<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: diklin.xml,v 1.14 2023/08/31 10:44:00 revo Exp $">
<kap>
  <rad>diklin</rad>/a
</kap>
<drv mrk="diklin.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">BOT</uzo>
  <uzo tip="klr">(pri plantoj)</uzo>
  <snc mrk="diklin.0a.BOT">
    <dif>
      Havanta nur unuseksajn florojn, t.e. havanta apartajn inajn kaj malinajn
      florojn &ccirc;u sur sama planto (<ref tip="sub" cel="monoik.0a">monoika</ref>)
      a&ubreve; &ccirc;u sur diversaj plantoj
      (<ref tip="sub" cel="dioik.0a">dioika</ref>):
      <ekz>
        kamemoro <klr>[&#x2026;]</klr> estas moltiga sta&ubreve;do, <tld/>a (unuseksa) planto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=141721">Kamemoro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="androg.0a">androgina</ref> (monoklina)
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>dvoudom&yacute;</trd>,
    <trd>dvoud&ecaron;lo&zcaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>eingeschlechtlich</trd>,
    <trd>eingeschlechtig</trd>,
    <trd>diklin</trd>
  </trdgrp>
  <trd lng="es">diclino</trd>
  <trd lng="fr">dicline</trd>
  <trd lng="ja">&#x96CC;&#x96C4;&#x7570;&#x82B1;&#x306E;  [<ind>&#x3057;&#x3086;&#x3046;&#x3044;&#x304B;&#x306E;</ind>]</trd>
  <trdgrp lng="pt">
    <trd>diclino</trd>,
    <trd>dicl&iacute;nico</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_d;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_p;&c_o;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_n;&c_o;&c_p;&c_o;&c_l;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">dvojdom&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_z;&c_d;&c_ib;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_a;&c_t;&c_je;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_r;&c_ib;&c_z;&c_n;&c_o;&c_s;&c_t;&c_a;&c_t;&c_je;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
  <adm>
    Eble mankas ankora&ubreve; artikolo &leftquot;monoklina&rightquot;.
  </adm>
</drv>
</art>
