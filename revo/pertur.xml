<vortaro>
<art mrk="$Id$">
<kap>
  <rad>perturb</rad>/o
</kap>
<drv mrk="pertur.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MED</uzo>
    <dif>
      Malordo en la homa korpo pro malbona funkcio de iu organo.
    </dif>
    <trdgrp lng="hu">
      <trd>zavar</trd>,
      <trd>diszfunkci&oacute;</trd>
    </trdgrp>
    <trd lng="pt">perturba&ccedil;&atilde;o</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_s;&c_s;&c_t;&c_r;&c_o;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>,
      <trd>&c_d;&c_i;&c_s;&c_f;&c_u;&c_n;&c_k;&c_c;&c_i;&c_ja;</trd>
    </trdgrp>    
  </snc>
  <snc>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Malordo en ma&scirc;ino.
    </dif>
    <trd lng="hu">&uuml;zemzavar</trd>    
    <trd lng="ru">&c_s;&c_b;&c_o;&c_j;</trd>
  </snc>
  <trd lng="de">St&ouml;rung</trd>
  <trd lng="nl">storing</trd>
  <trdgrp lng="pt">
    <trd>defeito</trd>,
    <trd>falha</trd>,
    <trd>engui&ccedil;o</trd>    
  </trdgrp>
</drv>

<drv mrk="pertur.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ka&ubreve;zi <tld/>ojn.
    </dif>
  </snc>
  <trd lng="de">st&ouml;ren</trd>
  <trd lng="nl">storen</trd>
  <trdgrp lng="hu">
    <trd>megzavar</trd>,
    <trd>zavart okoz</trd>,
    <trd>&uuml;zemzavart id&eacute;z el&odblac;</trd>
  </trdgrp>  
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_z;&c_v;&c_a;&c_t;&c_mol; <ind>&c_s;&c_b;&c_o;&c_j;</ind></trd>,
    <trd>&c_v;&c_y;&c_z;&c_v;&c_a;&c_t;&c_mol; <ind>&c_r;&c_a;&c_s;&c_s;&c_t;&c_r;&c_o;&c_j;&c_s;&c_t;&c_v;&c_o;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="pertur.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Havi <tld/>ojn.
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>zavar keletkezik</trd>,
    <trd>&uuml;zemzavar l&eacute;p fel</trd>
  </trdgrp>  
  <trd lng="nl">ge<ind>stoor</ind>d zijn</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_b;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_s;&c_t;&c_r;&c_o;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
