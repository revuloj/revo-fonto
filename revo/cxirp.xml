<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&ccirc;irp</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="cxirp.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Eligi akran, vibran sonon, kiel iuj insektoj, precipe cikadoj kaj
      griloj:
      <ekz>
        du griloj, jen unu, jen la alia, dormige <tld/>is post la forno
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 20a, p.
        123-124a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">striduler</trd>
  <trd lng="hu">ciripel</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_i;&c_r;&c_i;&c_k;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_je;&c_k;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
