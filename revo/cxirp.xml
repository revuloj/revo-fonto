<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cxirp.xml,v 1.25 2023/10/31 13:37:04 revo Exp $">
<kap>
  <rad>&ccirc;irp</rad>/i
  <fnt><bib>PVEF</bib></fnt>
</kap>

<drv mrk="cxirp.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="cxirp.0.KOMUNE">
    <dif>
      Frotsono de insekto a&ubreve; simila:
      <ekz>
        En &gcirc;ardeno pompas flora arbo, &parsl;
        Kie <tld/>as, aj, cikad' per harpo &parsl;
        <tld lit="&Ccirc;"/>! la mond' sin lulas en ebrio
        <fnt>
          <aut>&Ccirc;u Daorong</aut>
          <vrk>&Ccirc;eno de Embuskoj</vrk>
          <lok>en: Beletra Almanako, 2016-06 (26)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>zirp</trd>,
    <trd>tschilp</trd>
  </trdgrp>
  <trd lng="fr">cri-cri</trd>
</drv>

<drv mrk="cxirp.0i">
  <kap><tld/>i</kap>
  <snc mrk="cxirp.0i.soni">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Eligi akran, vibran sonon, kiel iuj insektoj, precipe cikadoj kaj
      griloj:
      <ekz>
        la griloj <tld/>adis
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        vokoj kaj revokoj flugis, kvaza&ubreve; &gcirc;oje <tld/>antaj birdoj
        <fnt>
          <bib>Lasta</bib>
          <lok>La Lasta</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>inte per alumeto, li lumigis rondan ladan numeron
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        Jano atendis &ccirc;u murmuron, &ccirc;u <tld/>an birdovo&ccirc;on
        <fnt>
          <bib>ChC</bib>
          <lok>1</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi forlasis <klr>[&#x2026;]</klr> la koleran <tld/>adon de l&#x2019; cikadoj
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#x2116;6</lok>
        </fnt>;
      </ekz>
      <ekz>
        du griloj, jen unu, jen la alia, dormige <tld/>is post la forno
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 20a, p.
        123-124a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frot.0sono.KOMUNE">frotsono</ref>,
      <ref cel="kricx.0i">kri&ccirc;i</ref>,
      <ref cel="pep.0i.bleki">pepi</ref>,
      <ref cel="tril.0i">trili</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_c;&c_v;&c_y;&c_r;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_c;&c_y;&c_r;&c_y;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_sh;&c_ch;&c_a;&c_b;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_t;&c_r;&c_a;&c_k;&c_a;&c_t;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>zirpen</trd>,
      <trd>tschilpen</trd>
    </trdgrp>
    <trd lng="en">chirp</trd>
    <trd lng="es">chirriar</trd>
    <trd lng="fa">&a_j;&f_y;&a_r;&a_j;&f_y;&a_r;
    &f_k;&a_r;&a_d;&a_n;</trd>
    <trdgrp lng="fr">
      <trd>striduler</trd>,
      <trd>gazouiller</trd>
    </trdgrp>
    <trd lng="he">&lamed;&tsadi;&yod;&yod;&ftsadi;</trd>
    <trd lng="hu">ciripel</trd>
    <trdgrp lng="it">
      <trd>stridere</trd>,
      <trd>frinire</trd>
    </trdgrp>
    <trdgrp lng="ja">
      <trd>&#x9CF4;&#x304F;  <pr>&#x306A;&#x304F;</pr></trd>,
      <trd>&#x3055;&#x3048;&#x305A;&#x308B;</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>tsjirpen</trd>,
      <trd>tjirpen</trd>
    </trdgrp>
    <trd lng="pl">&cacute;wierka&cacute;</trd>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_i;&c_r;&c_i;&c_k;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_t;&c_r;&c_je;&c_k;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="uk">
      <trd>&c_c;&c_v;&c_ib;&c_r;&c_ib;&c_n;&c_mol;&c_k;&c_a;&c_t;&c_i;</trd>,
      <trd>&c_s;&c_k;&c_r;&c_je;&c_k;&c_o;&c_t;&c_a;&c_t;&c_i;</trd>,
      <trd>&c_shch;&c_je;&c_b;&c_je;&c_t;&c_a;&c_t;&c_i;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x866B;&#x9E1F;&#x53EB;&#x58F0; <pr>ch&#xF3;ngni&#x1CE;oji&#xE0;osh&#x113;ng</pr></trd>,
    <trd>&#x557E; <pr>ji&#x16B;</pr></trd>,
    <trd>&#x566A; <pr>z&#xE0;o</pr></trd>,
    <trd>&#x5545; <pr>zh&#xE0;o</pr></trd>
  </trdgrp>
</snc>
    <snc mrk="cxirp.0i.sonigi">
    <gra><vspec>tr</vspec></gra>
    <uzo tip="stl">EVI</uzo>
    <dif>
      <tld lit="&Ccirc;"/>igi:
      <ekz>
        <nom>Zoja</nom> <tld/>is la alumeton
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia sensonora, maljunulina vo&ccirc;o kortu&scirc;e <tld/>adis ilin, dum la lulilo movi&gcirc;is
        <fnt>
          <bib>HsT</bib>
          <lok>Heredanto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
</drv>
</art>
