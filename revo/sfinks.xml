<art mrk="$Id: sfinks.xml,v 1.24 2024/03/24 20:21:56 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>sfinks</rad>/o
</kap>
<drv mrk="sfinks.0o">
  <kap><tld/>o</kap>
  <snc mrk="sfinks.0o.MIT">
    <dif>
      Mita <ref tip="super" cel="monstr.0o.BIO">monstro</ref> kun leona
      korpo kaj homa kapo:
    </dif>
    <subsnc mrk="sfinks.0o.MIT_egipta">
      <uzo tip="fak">MIT</uzo>
      <dif>
        &Ccirc;e la antikvaj egiptanoj, figurata en ku&scirc;a sinteno,
        simbolanta la sunon:
        <ekz>
          sur blanka kalka monteto estis klare videbla silueto de
          <tld/>o
          <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 19a</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="hu">szfinx</trd>
    </subsnc>
    <subsnc mrk="sfinks.0o.MIT_greka">
      <uzo tip="fak">MIT</uzo>
      <dif>
        &Ccirc;e la antikvaj grekoj kun virina brusto kaj kapo,
        forglutanta tiujn, kiuj ne kapablis diveni la enigmojn
        proponitajn de &gcirc;i.
      </dif>
      <trd lng="hu">Szphinx</trd>
    </subsnc>
    <trd lng="pl">Sfinks</trd>
  </snc>
  <snc mrk="sfinks.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <uzo tip="stl">RAR</uzo>
    <dif>
      Homo, kies pensojn kaj sentojn oni ne povas diveni.
    </dif>
    <trdgrp lng="hu">
      <trd>titokzatos alak</trd>,
      <trd>kif&uuml;rk&eacute;szhetetlen ember</trd>
    </trdgrp>
    <trd lng="pl">sfinks</trd>
  </snc>
  <trd lng="be">&c_s;&c_f;&c_ib;&c_n;&c_k;&c_s;</trd>
  <trdgrp lng="cs">
    <trd>sfinga</trd>,
    <trd>sfinx</trd>
  </trdgrp>
  <trd lng="de">Sphinx</trd>
  <trd lng="en">sphinx</trd>
  <trd lng="es">esfinge</trd>
  <trd lng="fr">sphinx</trd>
  <trdgrp lng="ja">
    <trd>&#x30B9;&#x30D5;&#x30A3;&#x30F3;&#x30AF;&#x30B9;</trd>,
    <trd>&#x8B0E;&#x3081;&#x3044;&#x305F;&#x4EBA;&#x7269;  <pr>&#x306A;&#x305E;&#x3081;&#x3044;&#x305F;&#x3058;&#x3093;&#x3076;&#x3064;</pr></trd>
  </trdgrp>
  <trd lng="nl">sfinx</trd>
  <trd lng="ru">&c_s;&c_f;&c_i;&c_n;&c_k;&c_s;</trd>
  <trdgrp lng="sk">
    <trd>sfinga</trd>,
    <trd>sfinx</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_f;&c_ib;&#x43D;&#x43A;&c_s;</trd>
  <trdgrp lng="zh">
    <trd>&#x4EBA;&#x5934;&#x72EE;&#x8EAB; <pr>r&#xE9;nt&#xF3;ush&#x12B;sh&#x113;n</pr></trd>,
    <trd>&#x4EBA;&#x982D;&#x7345;&#x8EAB; <pr>r&#xE9;nt&#xF3;ush&#x12B;sh&#x113;n</pr></trd>,
    <trd>&#x72EE;&#x8EAB;&#x4EBA;&#x9762;&#x50CF; <pr>sh&#x12B;sh&#x113;nr&#xE9;nmi&#xE0;nxi&#xE0;ng</pr></trd>,
    <trd>&#x7345;&#x8EAB;&#x4EBA;&#x9762;&#x50CF; <pr>sh&#x12B;sh&#x113;nr&#xE9;nmi&#xE0;nxi&#xE0;ng</pr></trd>,
    <trd>&#x65AF;&#x82AC;&#x514B;&#x65AF; <pr>s&#x12B;f&#x113;nk&#xE8;s&#x12B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sfinks.0a">
  <kap><tld/>a</kap>
  <snc mrk="sfinks.0a.FIG">
    <dif>
      Enigma.
    </dif>
    <refgrp tip="vid">
      <ref cel="mister.0a">mistera</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_g;&c_a;&c_d;&c_k;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_t;&c_a;&c_ja;&c_m;&c_n;&c_ib;&c_ch;&c_y;</trd>
  </trdgrp>
  <trd lng="en">sphinx-like</trd>
  <trd lng="cs">z&aacute;hadn&yacute; <klr>(p&rcaron;enes.)</klr></trd>
  <trd lng="de">r&auml;tselhaft</trd>
  <trdgrp lng="hu">
    <trd>titokzatos</trd>,
    <trd>kif&uuml;rk&eacute;szhetetlen</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30B9;&#x30D5;&#x30A3;&#x30F3;&#x30AF;&#x30B9;&#x306E;</trd>,
    <trd>&#x306A;&#x305E;&#x306E;</trd>
  </trdgrp>
  <trd lng="nl">raadselachtig</trd>
  <trdgrp lng="pl">
    <trd>tajemniczy</trd>,
    <trd>nieodgadniony</trd>
  </trdgrp>
  <trd lng="sk">z&aacute;hadn&yacute;</trd>
  <trdgrp lng="zh">
    <trd>&#x610F;&#x4E49;&#x6DF1;&#x8FDC; <pr>y&#xEC;y&#xEC;sh&#x113;nyu&#x1CE;n</pr></trd>,
    <trd>&#x610F;&#x7FA9;&#x6DF1;&#x9060; <pr>y&#xEC;y&#xEC;sh&#x113;nyu&#x1CE;n</pr></trd>,
    <trd>&#x83AB;&#x660E;&#x5176;&#x5999; <pr>m&#xF2;m&#xED;ngq&#xED;mi&#xE0;o</pr></trd>,
    <trd>&#x83AB;&#x6D4B;&#x9AD8;&#x6DF1; <pr>m&#xF2;c&#xE8;g&#x101;osh&#x113;n</pr></trd>,
    <trd>&#x83AB;&#x6E2C;&#x9AD8;&#x6DF1; <pr>m&#xF2;c&#xE8;g&#x101;osh&#x113;n</pr></trd>,
    <trd>&#x5927;&#x60D1;&#x4E0D;&#x89E3;&#x7684; <pr>d&#xE0;hu&#xF2;b&#xF9;ji&#x11B;de</pr></trd>,
    <trd>&#x6781;&#x6DF1; <pr>j&#xED;sh&#x113;n</pr></trd>,
    <trd>&#x6975;&#x6DF1; <pr>j&#xED;sh&#x113;n</pr></trd>,
    <trd>&#x795E;&#x79D8;&#x83AB;&#x6D4B; <pr>sh&#xE9;nm&#xEC;m&#xF2;c&#xE8;</pr></trd>,
    <trd>&#x795E;&#x79D8;&#x83AB;&#x6E2C; <pr>sh&#xE9;nm&#xEC;m&#xF2;c&#xE8;</pr></trd>,
    <trd>&#x83AB;&#x540D;&#x5176;&#x5999;&#x7684; <pr>m&#xF2;m&#xED;ngq&#xED;mi&#xE0;ode</pr></trd>,
    <trd>&#x83AB;&#x540D;&#x5176;&#x5999; <pr>m&#xF2;m&#xED;ngq&#xED;mi&#xE0;o</pr></trd>
  </trdgrp>
</drv>
</art>
