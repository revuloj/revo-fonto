<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sfinks.xml,v 1.21 2023/09/03 18:39:29 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>sfinks</rad>/o
</kap>
<drv mrk="sfinks.0o">
  <kap><tld/>o</kap>
  <snc mrk="sfinks.0o.MIT">
    <dif>
      Mita <ref tip="super" cel="monstr.0o.BIO">monstro</ref> kun leona
      korpo kaj homa kapo:
    </dif>
    <subsnc mrk="sfinks.0o.MIT_egipta">
      <uzo tip="fak">MIT</uzo>
      <dif>
        &Ccirc;e la antikvaj egiptanoj, figurata en ku&scirc;a sinteno,
        simbolanta la sunon:
        <ekz>
          sur blanka kalka monteto estis klare videbla silueto de
          <tld/>o
          <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 19a</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="hu">szfinx</trd>
    </subsnc>
    <subsnc mrk="sfinks.0o.MIT_greka">
      <uzo tip="fak">MIT</uzo>
      <dif>
        &Ccirc;e la antikvaj grekoj kun virina brusto kaj kapo,
        forglutanta tiujn, kiuj ne kapablis diveni la enigmojn
        proponitajn de &gcirc;i.
      </dif>
      <trd lng="hu">Szphinx</trd>
    </subsnc>
    <trd lng="pl">Sfinks</trd>
  </snc>
  <snc mrk="sfinks.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <uzo tip="stl">RAR</uzo>
    <dif>
      Homo, kies pensojn kaj sentojn oni ne povas diveni.
    </dif>
    <trdgrp lng="hu">
      <trd>titokzatos alak</trd>,
      <trd>kif&uuml;rk&eacute;szhetetlen ember</trd>
    </trdgrp>
    <trd lng="pl">sfinks</trd>
  </snc>
  <trd lng="be">&c_s;&c_f;&c_ib;&c_n;&c_k;&c_s;</trd>
  <trdgrp lng="cs">
    <trd>sfinga</trd>,
    <trd>sfinx</trd>
  </trdgrp>
  <trd lng="de">Sphinx</trd>
  <trd lng="en">sphinx</trd>
  <trd lng="es">esfinge</trd>
  <trd lng="fr">sphinx</trd>
  <trdgrp lng="ja">
    <trd>&#x30B9;&#x30D5;&#x30A3;&#x30F3;&#x30AF;&#x30B9;</trd>,
    <trd>&#x8B0E;&#x3081;&#x3044;&#x305F;&#x4EBA;&#x7269;  [<ind>&#x306A;&#x305E;&#x3081;&#x3044;&#x305F;&#x3058;&#x3093;&#x3076;&#x3064;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">sfinx</trd>
  <trd lng="ru">&c_s;&c_f;&c_i;&c_n;&c_k;&c_s;</trd>
  <trdgrp lng="sk">
    <trd>sfinga</trd>,
    <trd>sfinx</trd>
  </trdgrp>
  <trd lng="uk">&#x441;&#x444;&#x456;&#x43D;&#x43A;&#x441;</trd>
</drv>

<drv mrk="sfinks.0a">
  <kap><tld/>a</kap>
  <snc mrk="sfinks.0a.FIG">
    <dif>
      Enigma.
    </dif>
    <refgrp tip="vid">
      <ref cel="mister.0a">mistera</ref>
    </refgrp>
  </snc>
  <trd lng="en">sphinx-like</trd>
  <trd lng="cs">z&aacute;hadn&yacute; <klr>(p&rcaron;enes.)</klr></trd>
  <trd lng="de">r&auml;tselhaft</trd>
  <trdgrp lng="hu">
    <trd>titokzatos</trd>,
    <trd>kif&uuml;rk&eacute;szhetetlen</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30B9;&#x30D5;&#x30A3;&#x30F3;&#x30AF;&#x30B9;&#x306E;</trd>,
    <trd>&#x306A;&#x305E;&#x306E;</trd>
  </trdgrp>
  <trd lng="nl">raadselachtig</trd>
  <trdgrp lng="pl">
    <trd>tajemniczy</trd>,
    <trd>nieodgadniony</trd>
  </trdgrp>
  <trd lng="sk">z&aacute;hadn&yacute;</trd>
</drv>
</art>
