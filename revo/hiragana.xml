<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hiragana.xml,v 1.10 2022/04/30 08:42:41 revo Exp $">
<kap>
  <rad>hiragana</rad>/o <fnt><bib>NePiv</bib></fnt>,
  <var><kap><rad var="n">hiragan</rad>/o</kap></var>
</kap>
<drv mrk="hiragana.0o">
  <kap><tld/>o, <var><kap><tld var="n"/>o</kap></var></kap>
  <snc mrk="hiragana.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref tip="dif" cel="kana.ronda0o">Ronda kanao</ref>:
      <ekz>
        lernantoj de la japana tutegale devos lerni anka&ubreve;
        <tld/>on
        <fnt>
          <aut>Z. Lapcionak</aut>,
          <vrk><url ref="http://tech.groups.yahoo.com/group/revuloj/message/11337">por traduko en la
          japana</url></vrk>,
          <lok>diskutgrupo &leftquot;revuloj&rightquot;, 2007-08-27</lok>
        </fnt>;
      </ekz>
      <ekz>
        la dua urbo kies nomo estas en <tld var="n"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=431295"
              >Cugaru (urbo)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        litertipoj por la hebrea, malnovgreka, hieroglifa kaj japana (<tld/>&#8239;/&#8239;katakana)
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato1/monato-9506-lingvema.txt"
              >Por lingvemaj komputilemuloj...</url></vrk>
          <lok>95/06</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="katakana.0o.TIP">katakanao</ref>
    <trd lng="be">&c_h;&c_ib;&c_r;&c_a;&c_g;&c_a;&c_n;&c_a;</trd>
    <trd lng="de">Hiragana</trd>
    <trd lng="fa">&a_h;&f_y;&a_r;&a_A;&f_g;&a_A;&a_n;&a_A;</trd>
    <trd lng="fr">hiragana</trd>
    <trd lng="it">hiragana</trd>
    <trd lng="pl">hiragana</trd>
    <trd lng="pt">"<ind>hiragana</ind>"</trd>
  </snc>
</drv>
</art>

