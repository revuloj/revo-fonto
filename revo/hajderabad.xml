<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: hajderabad.xml,v 1.5 2023/11/15 16:45:17 afido Exp $">
    <kap>
      <rad>Hajderabad</rad>/o
    </kap>

<drv mrk="hajderabad.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="hajderabad.0o.barata">
    <dif>
      &Ccirc;efa <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      de <ref tip="malprt" cel="telangan.0o">Telangano</ref> (ekde 2014) en la sudo de <ref tip="malprt" cel="barat.B0o">Barato</ref>:
      <ekz>
        <klr>[&scirc;i]</klr> trovis laboron &ccirc;e usona firmao, kies filio en <tld/>o funkciigas
        <klr>[&#x2026;]</klr> telefonplusendan centron por servi al usonaj klientoj
        <fnt>
          <bib>Monato</bib>
          <aut>Probal Da&scirc;gupto</aut>
          <vrk><url ref="&Monato;/2004/008239.html">Publika sakrado</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        instruistoj de la tribaj lingvoj, lingvistoj de universitatoj en <tld/>o, la
        &ccirc;efurbo de <ref cel="andrapradesx.0o">Andraprade&scirc;o</ref>
        <fnt>
          <bib>Monato</bib>
          <aut>A. Giridhar Rao</aut>
          <vrk><url ref="&Monato;/2005/008714.php">Gepatralingva edukado por
          indi&gcirc;enoj</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="te">&#xC39;&#xC48;&#xC26;&#xC30;&#xC3E;&#xC2C;&#xC3E;&#xC26;&#xC4D;</trd>
  </snc>
  <snc mrk="hajderabad.0o.pakistana">
    <dif>
      Granda <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      &ccirc;e <ref cel="indus.0o">Induso</ref>, en la sudo de <ref tip="malprt" cel="pakistan.P0o">Pakistano</ref>,
      &ccirc;. 150&#x202F;km nordoriente de <ref cel="karacxi.0o">Kara&ccirc;io</ref>:
      <ekz>
        <tld/>o estis fondita en 1768 de <nom>Mian Ghulam</nom> &Scirc;aho Kalhoro en regiono kun pluraj
        montetoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=332423">Hajderabado (Pakistano)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="sd">&a_H;&a_y;&a_d;&a_r;&a_A_madda;&a_b;&a_A;&a_d; &a_s;&a_n;&#x68C;</trd>
  </snc>
  <trd lng="be">&c_H;&c_a;&c_j;&c_d;&c_e;&c_r;&c_a;&c_b;&c_a;&c_d;</trd>
  <trd lng="de">Hyderabad</trd>
  <trd lng="en">Hyderabad</trd>
  <trd lng="fr">Hyderabad</trd>
  <trd lng="ur">&a_H;&f_y;&a_d;&a_r; &a_A_madda;&a_b;&a_A;&a_d;</trd>
  <trdgrp lng="zh">
    <trd>&#x6D77;&#x5F97;&#x62C9;&#x5DF4; <pr>h&#x1CE;id&#xE9;l&#x101;b&#x101;</pr></trd>,
    <trd>&#x6D77;&#x5F97;&#x62C9;&#x5DF4; <pr>h&#x1CE;idel&#x101;b&#x101;</pr></trd>
  </trdgrp>
</drv>

  </art>

