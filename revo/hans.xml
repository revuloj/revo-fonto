<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hans.xml,v 1.23 2023/11/15 16:45:17 revo Exp $">
<kap>
  <rad>hans</rad>/o
</kap>
<drv mrk="hans.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      Komerca <ref tip="super" cel="asoci.0o.societo">asocio</ref> de negocistoj 
      a&ubreve; <ref tip="prt" cel="urb.0o">urbo</ref>j:
      <ekz>
        la plej konata estas la <ctl>Germana <tld lit="H"/>o</ctl>,
        kiu ligis inter la 13a kaj la 16a jarcentoj
        komercistojn el norde&ubreve;ropaj urboj
        <fnt><aut>Roland Rotsaert</aut>:
          <vrk>Mezepoka hanso-asocio revivi&gcirc;as</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2002a, numero 4a, p. 17a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="H"/>a Ligo <klr>[&#x2026;]</klr> kunigis &ccirc;. 200 urbojn en E&ubreve;ropo
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2011/010405.php">Bruas la mezepoko</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gild.0o">gildo</ref>,
      <ref cel="korpor.0o">korporacio</ref>,
      <ref cel="sindik1.0o">sindikato</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_n;&c_z;&c_a;</trd>
  <trd lng="bg">&c_h;&c_a;&c_n;&c_z;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>hansa</trd>,
    <trd>hanza</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hanse</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>corporaci&oacute;n</trd>,
    <trd>sociedad</trd>
  </trdgrp>
  <trd lng="fr">hanse</trd>
  <trd lng="hu">Hanza</trd>
  <trd lng="nl">Hansa</trd>
  <trd lng="pt">hansa</trd>
  <trd lng="ru">&c_g;&c_a;&c_n;&c_z;&c_a;</trd>
  <trd lng="sk">hanza</trd>
  <trd lng="uk">&c_g;&c_a;&#x43D;&c_z;&c_a;</trd>
  <trd lng="zh">&#x6C49;&#x8428;&#x540C;&#x76DF; <pr>h&#xE0;ns&#xE0;t&#xF3;ngm&#xE9;ng</pr></trd>
</drv>
</art>
