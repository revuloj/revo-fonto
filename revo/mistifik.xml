<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mistifik.xml,v 1.23 2024/01/26 17:40:07 revo Exp $">
<kap>
  <ofc>8</ofc><rad>mistifik</rad>/i <fnt><bib>PV</bib></fnt>,
  <var><kap><rad var="f">mistif</rad>/i <fnt><bib>PV</bib></fnt></kap></var>
</kap>
<drv mrk="mistifik.0i">
  <kap><tld/>i, <var><kap><tld var="f"/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="mistifik.0i.trompeti">
    <dif>
      <ref tip="super" cel="sxerc.0i">&Scirc;erci</ref> per malgrava trompo,
      tro uzante ies kredemon:
      <ekz>
        kiu kiun <tld/>as
        <fnt>
        <aut>Michel Duc Goninaz</aut>
        <vrk><url ref="&Monato;/2000/006947.html">Eks-Jugoslavio: 
          kiu kiun mistifikas?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 8</lok>
        </fnt>
      </ekz>    
      <ekz>
        je la sama horo, kiam la ministro ankora&ubreve; <tld/>is pri ETA, la polico petis de
        ju&gcirc;isto la devigajn arest-ordonojn
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2004/008300.html">Kronologio de kanajla
          mistifikado</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        malicaj koboldoj <klr>[&#x2026;]</klr> devojigas la
        karavanojn, elsekigas la putojn kaj dum la
        senlunaj noktoj senripozigas la <tld var="f"/>itajn
        kamelistojn
        <fnt>
          <aut>J. Ribillard</aut>,
          <vrk>Vagado sub palmoj</vrk>,
          <lok>Rotterdam, 1956</lok>
        </fnt>.      
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_s;&c_t;&c_y;&c_f;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>mystifikovat</trd>,
    <trd>obloudit</trd>,
    <trd>oklamat</trd>,
    <trd>pobl&aacute;znit</trd>,
    <trd>podv&aacute;d&ecaron;t</trd>,
    <trd>zm&aacute;mit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>foppen</trd>,
    <trd>narren</trd>,
    <trd>hinters <ind>Licht</ind> f&uuml;hren</trd>,
    <trd>an der <ind>Nase</ind> herumf&uuml;hren</trd>,
    <trd>auf den <ind>Arm</ind> nehmen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>fool</trd>,
    <trd>hoax</trd>,
    <trd>trick</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>timar</trd>,
    <trd>mistificar</trd>,
    <trd>embaucar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire une <ind>blague</ind></trd>,
    <trd>mystifier</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>r&aacute;szed</trd>,
    <trd>becsap</trd>,
    <trd>f&eacute;lrevezet</trd>,
    <trd>beugrat</trd>,
    <trd>k&ouml;d&ouml;s&iacute;t</trd>
    <trd>misztifik&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7159;&#x306B;&#x307E;&#x304F;  <pr>&#x3051;&#x3080;&#x308A;&#x306B;&#x307E;&#x304F;</pr></trd>,
    <trd>&#x304B;&#x3064;&#x3050;</trd>,
    <trd>&#x8FF7;&#x308F;&#x3059;  <pr>&#x307E;&#x3088;&#x308F;&#x3059;</pr></trd>
  </trdgrp>
  <trd lng="nl">foppen</trd>
  <trdgrp lng="pl">
    <trd>mistyfikowa&cacute;</trd>,
    <trd>wprowadzi&cacute; w b&lstroke;&aogonek;d</trd>,
    <trd>zwodzi&cacute;</trd>,
    <trd>zadrwi&cacute;</trd>,
    <trd>zakpi&cacute;</trd>
  </trdgrp> 
  <trdgrp lng="pt">
   <trd>mistificar</trd>,
   <trd>ludibriar</trd>,
   <trd>burlar</trd>   
  </trdgrp>
  <trdgrp lng="ro">
   <trd>mistifica</trd>,
    <trd>induce &icirc;n eroare</trd>,
    <trd>&icirc;n&#x219;ela</trd>,
    <trd>insinuare</trd>,
    <trd>batjocori</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_i;&c_s;&c_t;&c_i;&c_f;&c_i;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>mystifikova&tcaron;</trd>,
    <trd>oklama&tcaron;</trd>,
    <trd>pobl&aacute;zni&tcaron;</trd>,
    <trd>podv&aacute;dza&tcaron;</trd>,
    <trd>zm&aacute;mi&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_ib;&c_s;&c_t;&c_i;&c_f;&c_ib;&#x43A;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&#x43E;&c_z;&c_ib;&c_g;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x624B;&#x811A; <pr>sh&#x1D2;uji&#x1CE;o</pr></trd>,
    <trd>&#x624B;&#x8173; <pr>sh&#x1D2;uji&#x1CE;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mistifik.0a">
  <kap><tld/>a</kap>
  <snc mrk="mistifik.0a.trompeta">
    <dif>
      Ka&ubreve;zanta celitan trompi&gcirc;on a&ubreve; konfuzon:
      <ekz>
        mi estas normala simpla homo, sed la sorto kondukis min en &ccirc;i
        tiun labirinton de ege utilaj kaj foje <tld/>aj
        fenomenoj<fnt><aut>Will
        Green</aut>: <vrk>Telefona labirinto</vrk>,
        <bib>Monato</bib><lok>jaro 1994a, numero 1a, p.
        13a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ib;&c_s;&c_t;&c_y;&c_f;&c_ib;&c_k;&c_a;&c_t;&c_a;&c_r;&c_s;&c_k;&c_ib;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_m;&c_a;&c_n;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>klamn&yacute;</trd>,
    <trd>nepochopiteln&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>irref&uuml;hrend</trd>,
    <trd>verwirrend</trd>
  </trdgrp>
  <trd lng="en">tricky</trd>
  <trdgrp lng="es">
    <trd>bromista</trd>,
    <trd>mistificador</trd>,
    <trd>embaucador</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>blagueur<klr> (adj.)</klr></trd>,
    <trd>mystificateur<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="pl">mistyfikatorski</trd>
  <trd lng="ro">mistificator</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_m;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_b;&c_m;&c_a;&c_n;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>klamn&yacute;</trd>,
    <trd>nepochopite&lcaron;n&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="mistifik.0o">
  <kap><tld/>o</kap>
  <snc mrk="mistifik.0o.trompeto">
    <dif>
      &Scirc;erca <ref tip="super" cel="tromp.0o">trompo</ref>:
      <ekz>
        kun a&ubreve;reol' de <tld/>-mistiko / alflugu do, estrino de l' pedeloj
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
          <lok>Oni Malfermas La Butikon</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj misteroj kaj <tld/>oj subite havas plene kredeblan klarigon
        <fnt>
          <bib>Monato</bib>
          <aut>Pejno Simono</aut>
          <vrk><url ref="&Monato;/2013/011040.php">Toter Lehrer, guter
          Lehrer</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        li sukcesis travidi la multajn <tld/>ojn, kiuj kamufle
        &ccirc;irka&ubreve;vualas nian realan movadan historion
        <fnt><aut>Pejno Simono</aut>:
          <vrk>Faligas la fundamentojn de esperantismo (Ziko Marcus
          Sikosek, Esperanto sen mitoj)</vrk>,
          <bib>Monato</bib><lok>jaro 1999a,
          numero 6a, p. 27a</lok></fnt>;
      </ekz>
      <ekz>
        sen<tld/>igo de la naciaj propagandoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Platteau</aut>
          <vrk><url ref="&Monato;/2014/011376.php">&Scirc;ira retaksado de la
          konceptoj</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_s;&c_t;&c_y;&c_f;&c_ib;&c_k;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>klam&aacute;n&iacute;</trd>,
    <trd>mystifikace</trd>,
    <trd>podv&aacute;d&ecaron;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Irref&uuml;hrung</trd>,
    <trd>F&auml;lschung</trd>,
    <trd>Verwirrung</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>hoax</trd>,
    <trd>trick</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>truco</trd>,
    <trd>inocentada</trd>,
    <trd>farsa</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>blague<klr> (tromperie)</klr></trd>,
    <trd>canular</trd>,
    <trd>mystification</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x304B;&#x3064;&#x3050;&#x3053;&#x3068;</trd>,
    <trd>&#x4E00;&#x676F;&#x98DF;&#x308F;&#x305B;&#x308B;&#x3053;&#x3068;  <pr>&#x3044;&#x3063;&#x3071;&#x3044;&#x304F;&#x308F;&#x305B;&#x308B;&#x3053;&#x3068;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>mistyfikacja</trd>,
    <trd>zw&oacute;d</trd>,
    <trd>uwiedzenie</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>cioara vopsita</trd>,
    <trd>&icirc;n&#x219;el&abreve;ciune</trd>,
    <trd>seduc&#x21B;ie</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_i;&c_s;&c_t;&c_i;&c_f;&c_i;&c_k;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>klamanie</trd>,
    <trd>podv&aacute;dzanie</trd>
  </trdgrp>
  <trd lng="uk">&#x43C;&c_ib;&c_s;&c_t;&c_i;&c_f;&c_ib;&#x43A;&c_a;&c_c;&c_ib;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x82B1;&#x6D3B; <pr>hu&#x101;hu&#xF3;</pr></trd>,
    <trd>&#x82B1;&#x62DB; <pr>hu&#x101;zh&#x101;o</pr></trd>
  </trdgrp>
</drv>
</art>

