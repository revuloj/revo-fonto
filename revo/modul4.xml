<vortaro>
<art mrk="$Id$">
<kap>
  <rad>modulaci</rad>/i
  <fnt><aut>Hajpin Li</aut>: <vrk>Esperanto-korea vortaro</vrk>,
  <lok>1983</lok></fnt>
  <fnt><aut>A. Cherpillod</aut>:
  <vrk>Nepivaj vortoj</vrk>, <lok>1988</lok></fnt>
  <fnt><lok><url
ref="&KompLeks;/ME.html#MODULO-2">KompLeks</url></lok></fnt>
</kap>
<drv mrk="modul4.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="modul4.0i.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Variigi la tonojn.
    </dif>
  </snc>
  <snc mrk="modul4.0i.vocx">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Variigi, nuanci la vo&ccirc;on.
    </dif>
  </snc>
  <snc mrk="modul4.0i.TEL">
    <uzo tip="fak">TEL</uzo>
    <uzo tip="fak">ELE</uzo>
    <dif>
      Variigi la amplitudon, frekvencon a&ubreve; alian karakteriza&jcirc;on
de
      relative altfrekvenca oscilo
      <klr>(de la <em>portanta</em> ondo, kurento)</klr> la&ubreve; la
      variado de transsendota signalo (kies frekvencoj estas
      konsiderinde malpli grandaj):
      <ekz>
        <tld/>i frekvence, amplitude, faze.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_d;&c_u;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="hu">modul&aacute;l</trd>  
  <trd lng="nl">moduleren</trd>
  <trd lng="fr">moduler</trd>
  <trd
lng="ru">&c_m;&c_o;&c_d;&c_u;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd
>
</drv>

<drv mrk="modul4.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="M"/>ado, t.e. la ago <tld/>i kaj la te&hcirc;nikaj
      metodoj uzantaj <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_d;&c_u;&c_l;&c_ja;&c_c;&c_y;&c_ja;</trd>
  <trd lng="es">modulaci&oacute;n</trd>
  <trd lng="fr">modulation</trd>
  <trd lng="hu">modul&aacute;ci&oacute;</trd>
  <trd lng="nl">modulatie</trd>
  <trd lng="ru">&c_m;&c_o;&c_d;&c_u;&c_l;&c_ja;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sk">modul√°cia</trd>
</drv>
</art>
