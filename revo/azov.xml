<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Azov</rad>/o
    </kap>
    <drv mrk="azov.0o">
      <kap><tld/>o
        <fnt>PIV</fnt>
      </kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
	<dif>
          Urbeto en <ref tip="malprt" cel="rus.R0ujo">Rusujo</ref>,
          &ccirc;e la elfluejo de Dono en la <tld/>an Maron
          (35&ring;25&#x2032;E, 47&ring;03&#x2032;N).
	</dif>
      </snc>
      <trd lng="ru">&c_A;&c_z;&c_o;&c_v;</trd>
    </drv>
    <drv mrk="azov.0aMaro">
      <kap><tld/>a Maro
        <fnt>PIV</fnt>
      </kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
	<dif>
          Interna maro Orient-<ref cel="euxrop1.0a">E&ubreve;ropa</ref>,
          inter <ref cel="rus.R0ujo">Rusujo</ref> kaj Ukrainujo,
          norde de la Nigra Maro.
	</dif>
      </snc>
      <trd lng="en">Sea of <ind>Azov</ind></trd>
      <trd lng="ru">&c_A;&c_z;&c_o;&c_v;&c_s;&c_k;&c_o;&c_je; &c_m;&c_o;&c_r;&c_je;</trd>
    </drv>
  </art>

