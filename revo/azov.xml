  <trd lng="be">&c_A;&c_z;&c_o;&c_w;</trd>
  <trd lng="ca">Azov</trd>
  <trd lng="de">Asow</trd>
  <trd lng="en">Azov</trd>
  <trd lng="fr">Azov</trd>
  <trd lng="hu">Azov</trd>
  <trd lng="it">Azov</trd>
  <trd lng="ru">&c_A;&c_z;&c_o;&c_v;</trd>
</drv>

</art>
