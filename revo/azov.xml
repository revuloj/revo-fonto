<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Azov</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="azov.0o">
  <kap><tld/>o</kap>
  <snc mrk="azov.0o.GEOG">
  <uzo tip="fak">GEOG</uzo>
    <dif>
      Urbeto en <ref tip="malprt" cel="rus.R0ujo">Rusujo</ref>,
      &ccirc;e la elfluejo de Dono en la <tld/>an Maron
      (35&ring;25&#8242;E, 47&ring;03&#8242;N).
    </dif>
  </snc>
  <trd lng="be">&c_A;&c_z;&c_o;&c_w;</trd>
  <trd lng="de">Asow</trd>
  <trd lng="en">Azov</trd>
  <trd lng="fr">Azov</trd>
  <trd lng="hu">Azov</trd>
  <trd lng="ru">&c_A;&c_z;&c_o;&c_v;</trd>
</drv>

<drv mrk="azov.0aMaro">
  <kap><tld/>a Maro <fnt><bib>PIV1</bib></fnt></kap>
  <snc mrk="azov.0aMaro.GEOG">
  <uzo tip="fak">GEOG</uzo>
    <dif>
      Interna maro Orient-<ref cel="euxrop1.0a">E&ubreve;ropa</ref>,
      inter <ref cel="rus.R0ujo">Rusujo</ref> kaj Ukrainujo,
      norde de la Nigra Maro.
    </dif>
  </snc>
  <trd lng="be">
  &c_A;&c_z;&c_o;&c_w;&c_s;&c_k;&c_a;&c_je; &c_m;&c_o;&c_r;&c_a;</trd>
  <trd lng="de">Asowsches <ind>Meer</ind></trd>
  <trd lng="en">Sea of <ind>Azov</ind></trd>
  <trd lng="fr">Mer d'Azov</trd>
  <trd lng="hu">Azovi-tenger</trd>
  <trd lng="pl">Morze <ind>Azowskie</ind></trd>
  <trd lng="ru">
  &c_A;&c_z;&c_o;&c_v;&c_s;&c_k;&c_o;&c_je; &c_m;&c_o;&c_r;&c_je;</trd>
</drv>
</art>
