<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Azov</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="azov.0o">
  <kap><tld/>o</kap>
  <snc mrk="azov.0o.GEOG">
  <uzo tip="fak">GEOG</uzo>
    <dif>
      Urbeto en <ref tip="malprt" cel="rus.R0ujo">Rusujo</ref>,
      &ccirc;e la elfluejo de Dono en la <tld/>an Maron
      (35&#730;25&minute;E, 47&#730;03&minute;N).
    </dif>
  </snc>
  <trd lng="be">&c_A;&c_z;&c_o;&c_w;</trd>
  <trd lng="ca">Azov</trd>
  <trd lng="de">Asow</trd>
  <trd lng="en">Azov</trd>
  <trd lng="fr">Azov</trd>
  <trd lng="hu">Azov</trd>
  <trd lng="it">Azov</trd>
  <trd lng="ru">&c_A;&c_z;&c_o;&c_v;</trd>
</drv>

<drv mrk="azov.0a_Maro">
  <kap>
    <tld/>a Maro
  </kap>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:maroj" cel="mar.0o.parto">Maro</ref>,
      nomita la&ubreve; la urbo <tld/>o, ligita kun la <ref cel="mar.NigraM0o">Nigra Maro</ref>,
      per la Ker&ccirc;a markolo:
      <ekz>
        forestas simila traktato pri maraj limoj en <tld/>a kaj Nigra Maroj
        <fnt>
          <bib>Monato</bib>
          <aut>Sergeo Jurji&ccirc;</aut>
          <vrk><url ref="https://www.monato.be/2009/009713.php"
              >Necesas senlima pacienco</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/c/c2/Bm_krim.jpg/800px-Bm_krim.jpg"
      prm="PD"
      ><tld/>a Maro kun la duoninsulo Krimeo
        <fnt>
          <lok><url ref="&WCW;/File:Bm_krim.jpg"
              >https://commons.wikimedia.org/wiki/File:Bm_krim.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="de">Asowsches Meer</trd>
</drv>

</art>
