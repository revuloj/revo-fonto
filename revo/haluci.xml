<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>halucin</rad>/o
</kap>
<drv mrk="haluci.0o">
  <kap><tld/>o, halucinacio<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Stato de neda&ubreve;ra frenezeco, en kiu oni perceptas ian sentuma&jcirc;on,
      kvankam ekzistas neniu ekstera objekto kapabla &gcirc;in produkti; senteraro.
    </dif>
  </snc>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_l;&c_ju;&c_c;&c_i;&c_n;&c_a;&c_c;&c_i;&c_ja;</trd>
</drv>
<drv mrk="haluci.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Ka&ubreve;zita de <tld/>o, simila al <tld/>o:
      <ekz><tld/>a teruro <klr>(f)</klr>;</ekz>
      <ekz><tld/>a filmo;</ekz>
      <ekz><tld/>a parado de fantomoj.</ekz>
    </dif>
  </snc>
</drv>
<drv mrk="haluci.0ato0ulo">
  <kap><tld/>ato, <tld/>ulo</kap>
  <snc>
    <dif>
      Malsanulo, kiu sentas <tld/>ojn. 
    </dif>
  </snc>
</drv>
<drv mrk="haluci.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Senterarigi iun, ka&ubreve;zi <tld/>on al iu. 
    </dif>
  </snc>
  <trd lng="ru">&c_v;&c_y;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol; &c_g;&c_a;&c_l;&c_l;&c_ju;&c_c;&c_i;&c_n;&c_a;&c_c;&c_i;&c_i;</trd>
</drv>
<drv mrk="haluci.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Senti <tld/>ojn.  
    </dif>
  </snc>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_l;&c_ju;&c_c;&c_i;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
<!--
$Log$
