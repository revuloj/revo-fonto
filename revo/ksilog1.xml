    <trd>xilogravura</trd>,
    <trd>xilografia</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_s;&c_i;&c_l;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>,
