<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ksilografi</rad>/o <fnt><bib>PV</bib></fnt>
  <fnt>Z</fnt>
</kap>
<drv mrk="ksilog1.0o">
  <kap><tld/>o</kap>
  <snc mrk="ksilog1.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Procedo de <ref tip="super" cel="pres.0arto.TIP">presarto</ref>,
      en
      kiu reliefan lignogravura&jcirc;on oni premas al surfaco por produkti
      bildon a&ubreve; tekston.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>d&rcaron;evoryt</trd>,
    <trd>d&rcaron;evorytectv&iacute;</trd>
  </trdgrp>
  <trd lng="de">Holzschneidekunst</trd>
  <trdgrp lng="en">
    <trd>woodcut</trd>,
    <trd>xylography <klr>(seldom)</klr></trd>
  </trdgrp>
  <trd lng="fr">xylographie<klr tip="amb"> (proc&eacute;d&eacute;)</klr></trd>
  <trdgrp lng="hu">
    <trd>fametsz&eacute;s</trd>,
    <trd>xilogr&aacute;fia</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>xilogravura</trd>,
    <trd>xilografia</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_s;&c_i;&c_l;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_v;&c_ju;&c_r;&c_a; &c_n;&c_a; &c_d;&c_je;&c_r;&c_je;&c_v;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">drevoryt</trd>
  <trdgrp lng="sv">
    <trd>xylografi</trd>,
    <trd>tr&auml;gravyr</trd>,
    <trd>tr&auml;stick</trd>
  </trdgrp>
</drv>

<drv mrk="ksilog1.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="ksilog1.0isto.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Artisto pri <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>woodcut artist</trd>,
    <trd>block-cutter</trd>
  </trdgrp>
  <trd lng="fr">xylographe</trd>
  <trdgrp lng="it">
    <trd>silografo</trd>,
    <trd>xilografo</trd>
  </trdgrp>
  <trd lng="sv">xylograf</trd>
</drv>
</art>
