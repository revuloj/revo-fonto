<art mrk="$Id: peged.xml,v 1.18 2024/02/17 06:43:05 revo Exp $">
<kap>
  <rad>peged</rad>/oj
</kap>
<drv mrk="peged.0oj">
  <kap><tld/>oj</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref tip="malprt" cel="peg.0oformaj">pegoformaj
      birdoj</ref> (<trd lng="la">Picidae</trd>):
      <ekz>
        pegedoj <klr>[&#x2026;]</klr> nutras sin per la insektoj trovitaj en la putrinta ligno,
        la arb&scirc;eloj a&ubreve; surtere, tiucele ili frapas sur ligno por ke la bruo elirigu la
        insektojn el siaj truoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16785">Pegedoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
       <ref cel="peg.0o.genro">pego</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_ja;&c_t;&c_l;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_k;&c_malmol;&c_l;&c_v;&c_a;&c_ch;&c_i;</trd>
  <trd lng="ca">p&iacute;cids</trd>
  <trd lng="cs">datlovit&iacute; <klr>(&ccaron;ele&dcaron;)</klr></trd>
  <trd lng="de">Spechte<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="en">woodpeckers <klr tip="ind">(family)</klr></trd>
  <trd lng="fr">Picid&eacute;s</trd>
  <trd lng="hu">hark&aacute;lyf&eacute;l&eacute;k</trd>
  <trd lng="ja">&#x30AD;&#x30C4;&#x30C4;&#x30AD;&#x79D1;  <pr>&#x30AD;&#x30C4;&#x30C4;&#x30AD;&#x304B;</pr></trd>
  <trd lng="nl">spechten<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pl">dzi&eogonek;cio&lstroke;owate</trd>
  <trd lng="pt">piciformes</trd>
  <trd lng="ro">picide</trd>
  <trd lng="ru">&c_d;&c_ja;&c_t;&c_l;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">&dcaron;atlovit&iacute;</trd>
  <trd lng="uk">&#x434;&#x44F;&#x442;&#x43B;&#x43E;&#x432;&#x456;</trd>
  <trdgrp lng="zh">
    <trd>&#x5544;&#x6728;&#x9CE5;&#x79D1; <pr>zhu&#xF3;m&#xF9;ni&#x1CE;ok&#x113;</pr></trd>,
    <trd>&#x5544;&#x6728;&#x9E1F;&#x79D1; <pr>zhu&#xF3;m&#xF9;ni&#x1CE;ok&#x113;</pr></trd>
  </trdgrp>
</drv>
</art>
