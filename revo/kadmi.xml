<art mrk="$Id$">
<kap>
  <rad>kadmi</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kadmi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
atomnumero 48, atompezo 112,40, simbolo Cd. Blanka metalo, uzata por alojoj
kaj pro siaj saloj.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_d;&c_m;&c_ib;&c_j;</trd>     
  <trd lng="br">kadmiom</trd>
  <trd lng="de">Cadmium</trd>
  <trd lng="en">cadmium</trd>
  <trd lng="es">cadmio</trd>
  <trd lng="fr">cadmium</trd>
  <trd lng="he">&qof;&dalet;&mem;&yod;&vav;&fmem;</trd>
  <trd lng="hu">kadmium</trd>
  <trd lng="nl">cadmium</trd>
  <trd lng="ru">&c_k;&c_a;&c_d;&c_m;&c_i;&c_j;</trd>
</drv>
</art>
