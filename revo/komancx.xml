<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>koman&ccirc;</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="komancx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="indian.0o">Indiano</ref> el popolo vivinta
      &ccirc;asiste en la usona ebena&jcirc;o, nun restanta en Oklahomo:
      <ekz>
        tie vivis la apa&ccirc;oj <klr>[...]</klr>, kiujn tamen 
        &ccirc;. 1750 p.K. for&ccirc;asis timiga
        popolo, la koman&ccirc;oj
        <fnt><aut>Tazio Carlevaro</aut>: <vrk>Skize pri
        indianaj kulturoj</vrk>, <lok>Literatura foiro, numero 115a,
        1998-10, p. 32-37a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <adm>
    Termino nedifinita en la difino: Oklahomo.
    <aut>MB</aut>
  </adm>
  <trd lng="be">&c_k;&c_a;&c_m;&c_a;&c_n;&c_ch;</trd>
  <trd lng="de">Komantsche</trd>
  <trdgrp lng="fr">
    <trd>comanche<klr tip="ind"> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_a;&c_n;&c_ch;</trd>
</drv>

<drv mrk="komancx.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al la <tld/>oj.
    
    </dif>
  </snc>
  <trd lng="de">komantschisch</trd>
  <trdgrp lng="fr">
    <trd>comanche<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_a;&c_n;&c_ch;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>

