<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>pozitron</rad>/o
</kap>
<drv mrk="pozitr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <ref tip="dif" cel="poziton.0o">pozitono</ref>
    <ekz>
      la unua antipartiklo, kiun observis en 1932 la amerika fizikisto
      Carl
      D. Anderson inter la trakoj de kosma radiado en nubkamero, estis
      la
      <tld/>o<fnt><aut>OH</aut>: <vrk>&Ccirc;u antimaterio en la
      universo?</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 2a, p.
      23a</lok></fnt>.
    </ekz>
    <trd lng="be">&c_p;&c_a;&c_z;&c_ib;&c_t;&c_r;&c_o;&c_n;</trd>
    <trdgrp lng="de">
      <trd>Positon</trd>,
      <trd>Positron</trd>
    </trdgrp>
    <trd lng="en">positron</trd>
    <trdgrp lng="fa">
      <trd>&f_p;&a_w;&a_z;&f_y;&a_t;&a_r;&a_w;&a_n;</trd>,
      <trd>&a_A;&a_l;&f_k;&a_t;&a_r;&a_w;&a_n; &a_m;&a_th;&a_b;&a_t;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>positon</trd>,
      <trd>positron</trd>
    </trdgrp>
    <trd lng="hu">pozitron</trd>
    <trd lng="nl">positron</trd>
    <trd lng="pt">p&oacute;sitron</trd>
    <trd lng="ru">&c_p;&c_o;&c_z;&c_i;&c_t;&c_r;&c_o;&c_n;</trd>
  </snc>
</drv>
</art>
