<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>bus</rad>/o
</kap>
<drv mrk="bus.0o">
  <kap><tld/>o</kap>
  <snc mrk="bus.0o.AUT">
    <uzo tip="fak">AUT</uzo>
    <ref tip="dif" cel="auxtob1.0o">a&ubreve;tobuso</ref>
    <ekz>
      la <tld/>o komencos sian veturon, kiam enestos sufi&ccirc;e da
      pasa&gcirc;eroj
      <fnt>
      <aut>Donald Broadribb</aut>,
      <vrk>Afrikanoj vidataj deekstere</vrk>,
      <bib>Monato</bib>,
      <lok>jaro 2002a, numero 3a, p. 24a</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_a;&c_w;&c_t;&c_o;&c_b;&c_u;&c_s;</trd>
  <trdgrp lng="de">
    <trd>Bus</trd>,
    <trd>Omnibus</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>cami&oacute;n</trd>,
    <trd>autob&uacute;s</trd>
  </trdgrp>
  <trd lng="en">bus</trd>
  <trd lng="fr">bus</trd>
  <trd lng="he">&alef;&vav;&tet;&vav;&bet;&vav;&samekh;</trd>
  <trd lng="hu">busz</trd>
  <trdgrp lng="nl">
    <trd>bus</trd>,
    <trd>autobus</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_v;&c_t;&c_o;&c_b;&c_u;&c_s;</trd>
  <trd lng="sk">autobus</trd>
  <trd lng="sv">buss</trd>
</drv>

<drv mrk="bus.trole0o">
  <kap>trole<tld/>o</kap>
  <snc mrk="bus.trole0o.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      Elektra buso, kiu kurenton ricevas per surtegmentaj stangoj (troleo):
      <ekz>
        inter la vetkurantaj reflektoroj aperis la okuloj de la
        trole<tld/>o
        <fnt>
        <aut>Pavel Stan&ccirc;ev, trad. Vedi</aut>,
        <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Noveloj/kolora_knabino.html">La kolora knabino</url></vrk>,
        <lok>Paco, 1989-2, p. 8a-9a</lok>
        </fnt>;
      </ekz>
      <ekz>
        La unua trole<tld/>o veturis en 1882 en Berlino
        <fnt>
        <bib>Viki</bib>, 
        <lok>artikolo &leftquot;trolebuso&rightquot;, 2005-10-19</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="super">
      <ref cel="auxtob1.0o.AUT">a&ubreve;tobuso</ref>
    </refgrp>
  </snc>
  <trd lng="fr">trolleybus</trd>
</drv>
</art>
