<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sism.xml,v 1.23 2023/08/21 13:29:33 revo Exp $">
<kap>
  <rad>sism</rad>/a <fnt><bib>PV</bib></fnt>,
  <var><kap><rad var="e">seism</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>
<drv mrk="sism.0o">
  <kap><tld/>o,
  <var><kap><tld var="e"/>o</kap></var></kap>
  <snc mrk="sism.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      <ref tip="dif" cel="trem.ter0o">Tertremo</ref>:
      <ekz>
        dum tia <tld/>o, plato de la submara planko malsupreniras,
        rilate al alia plato
        <fnt>
        <bib>Viki</bib>
        <vrk><url ref="&Viki;/Cunamo">
        Cunamo</url></vrk>
        <lok>2019-09-09</lok>
        </fnt>;
      </ekz>
      <ekz>
        la mastrumanta firmao de la atoma areo <klr>[&#8230;]</klr> indikis post la du <tld var="e"/>oj
        ke &gcirc;i ne konstatis materialajn konsekvencojn
        <fnt>
          <aut>-</aut>
          <vrk><url
          ref="http://neniammilitointerni.over-blog.com/2014/09/japanio-du-fortaj-tertremoj-apud-la-centralo-fukusimo-dajci.html">
          Japanio : du fortaj tertremoj apud la centralo Fuku&scirc;imo Daj&ccirc;i</url></vrk>
          <lok>Neniam milito inter ni, 2014-09-28</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="sismog.0o">sismografo</ref>,
    <ref cel="sismol.0o.SCI">sismologio</ref>
  </refgrp>
  <trd lng="be">&c_z;&c_je;&c_m;&c_l;&c_ja;&c_t;&c_r;&c_u;&c_s;</trd>
  <trd lng="ca">sisme</trd>
  <trd lng="cs">zem&ecaron;t&rcaron;esen&iacute;</trd>
  <trd lng="fr">s&eacute;isme</trd>
  <trd lng="pl">trz&eogonek;sienie ziemi</trd>
  <trd lng="sk">zemetrasenie</trd>
  <trd lng="uk">&c_z;&c_je;&#x43C;&#x43B;&c_je;&c_t;&c_r;&c_u;&c_s;</trd>
</drv>

<drv mrk="sism.0a">
  <kap><tld/>a,
  <var><kap><tld var="e"/>a</kap></var></kap>
  <snc mrk="sism.0a.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Rilatanta al la <ref tip="vid" cel="trem.ter0o">tertremoj</ref>
      kaj iliaj efikoj:
      <ekz>
        &leftquot;El <tld/>a zono&rightquot;
        <klr>(poemaro de <nom>Mikaelo Gi&scirc;pling</nom>)</klr>
        <fnt>
        <aut>Gi&scirc;pling M.</aut>
        <vrk>El sisma zono</vrk>
        <lok>1994</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_j;&c_s;&c_mol;&c_m;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">s&iacute;smic</trd>
  <trd lng="de">seismisch</trd>
  <trd lng="en">seismic</trd>
  <trd lng="fr">sismique</trd>
  <trd lng="hu">szeizmikus</trd>
  <trd lng="nl">seismisch</trd>
  <trd lng="pl">sejsmiczny</trd>
  <trd lng="ru">&c_s;&c_je;&c_j;&c_s;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="uk">&c_s;&c_je;&c_j;&c_s;&#x43C;&c_ib;&c_ch;&#x43D;&c_i;&c_j;</trd>
</drv>
</art>
