<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Babel</rad>/o <fnt>Z</fnt> <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="babel.B0o">
  <kap><tld/>o <fnt><bib>ZR</bib></fnt></kap>
  <snc mrk="babel.B0o.BIB">
    <uzo tip="fak">BIB</uzo>
    <dif>
      En la Hebrea Biblio, la urbego en <ctl>lando &Scirc;inar</ctl>,
      identigata kun
      <ref tip="sin" cel="babilon.B0o">Babilono</ref>:
      <ekz>
        kaj la komenco de lia regno estis <tld/> kaj Ere&hcirc; kaj Akad
        kaj Kalne en la lando &Scirc;inar
        <fnt><bib>MT</bib>, <lok>&amp;Gen ;10:10.</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_B;&c_a;&c_b;&c_ib;&c_l;&c_jo;&c_n;</trd>
  <trd lng="bg">&c_V;&c_a;&c_v;&c_i;&c_l;&c_o;&c_n;</trd>
  <trd lng="ca">Babel</trd>
  <trdgrp lng="de">
      <trd>Babel</trd>,
      <trd>Babylon</trd>
  </trdgrp>
  <trd lng="en">Babel</trd>
  <trd lng="es">Babel</trd>
  <trd lng="fa">&a_b;&a_A;&a_b;&a_kasra;&a_l;</trd>
  <trd lng="fr">Babel</trd>
  <trd lng="he">&bet;&bet;&lamed;</trd>
  <trd lng="hu">B&aacute;bel</trd>
  <trd lng="id">Babel</trd>
  <trd lng="it">Babele</trd>
  <trd lng="nl">Babel</trd>
  <trd lng="pl">Babel</trd>
  <trd lng="ru">&c_V;&c_a;&c_v;&c_i;&c_l;&c_o;&c_n;</trd>
  <trdgrp lng="sv">
    <trd>Babel</trd>,
    <trd>Babylon</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>Babil</trd>,
    <trd>Babilon</trd>
  </trdgrp>
</drv>

<drv mrk="babel.0turo">
  <kap><tld/>turo</kap>
  <snc mrk="babel.0turo.FRAZ">
    <uzo tip="fak">BIB</uzo>
    <uzo tip="stl">FRAZ</uzo>
    <uzo tip="fak">ESP</uzo>
    <dif>
      Simbolo de la lingva &hcirc;aoso, aludo al mito el la Hebrea Biblio.
    </dif>
    <trd lng="be">&c_B;&c_a;&c_b;&c_ib;&c_l;&c_jo;&c_n;&c_s;&c_k;&c_a;&c_ja; &c_v;&c_je;&c_zh;&c_a;</trd>
    <trd lng="bg">&c_V;&c_a;&c_v;&c_i;&c_l;&c_o;&c_n;&c_s;&c_k;&c_a; &c_k;&c_u;&c_l;&c_a;</trd>
    <trd lng="ca">torre de Babel</trd>
    <trdgrp lng="de">
      <trd>Turm von <ind>Babel</ind></trd>,
      <trd>Turm zu <ind>Babel</ind></trd>
    </trdgrp>
    <trd lng="en">Tower of Babel</trd>
    <trd lng="es">Torre de Babel</trd>
    <trd lng="fa">&a_b;&a_r;&a_j; &a_b;&a_A;&a_b;&a_kasra;&a_l;</trd>
    <trd lng="fr">tour de Babel</trd>
    <trd lng="he">&mem;&gimel;&dalet;&lamed; &bet;&bet;&lamed;</trd>
    <trdgrp lng="hu">
      <trd>B&aacute;bel tornya</trd>,
      <trd>b&aacute;beli torony</trd>
    </trdgrp>
    <trd lng="id">Menara Babel</trd>
    <trd lng="it">Torre di <ind>Babele</ind></trd>
    <trd lng="nl">toren van Babel</trd>
    <trd lng="pl">wie&zdot;a Babel</trd>
    <trd lng="ru">&c_V;&c_a;&c_v;&c_i;&c_l;&c_o;&c_n;&c_s;&c_k;&c_o;&c_je; &c_s;&c_t;&c_o;&c_l;&c_p;&c_o;&c_t;&c_v;&c_o;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
    <trd lng="sk">Babylonsk&aacute; ve&zcaron;a</trd>
    <trd lng="sv">Babels torn</trd>
    <trd lng="tr">Babil kulesi</trd>
    <trd lng="uk">&c_V;&c_a;&c_v;&c_i;&c_l;&c_o;&c_n;&c_s;&c_mol;&c_k;&c_a; &c_v;&c_je;&c_zh;&c_a;</trd>
  </snc>
  <trd lng="cs">Babylonská věž</trd>
</drv>
</art>

