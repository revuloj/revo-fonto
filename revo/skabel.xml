<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: skabel.xml,v 1.8 2025/04/05 19:27:39 afido Exp $">
    <kap>
      <rad>skabel</rad>/o
    <fnt><bib>PVEF</bib></fnt></kap>

<drv mrk="skabel.0o">
  <kap><tld/>o</kap>

  <snc>
    <dif>
      Malalta <ref tip="lst" lst="voko:mebloj" cel="tabure.0o">tabureto</ref>
      por sidante apogi la piedojn;
      <ref tip="dif" cel="benk.pied0eto">piedbenketo</ref>:
      <ekz>
        mia hundo sidis kun mi, kaj sur skabeloj unu kato &ccirc;iuflanke
        <fnt>
          <aut>Daniel Defoe, trad. Pastro A. Krafft </aut>
          <vrk><url ref="&Tekstaro;?nomo=robinsono-kruso">Robinsono Kruso</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi infana&gcirc;e tiom da fojoj sidadis sur <tld/>o en eta klostra &ccirc;elo
        <fnt>
          <aut>Hjalmar S&ouml;derberg, trad. Sten Johansson </aut>
          <vrk><url ref="&Tekstaro;?nomo=la-kiso">La Kiso</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la granda &scirc;tonplato estis definitive disigita de la skuadoj <klr>[&#8230;]</klr>, kiam
        li uzis &gcirc;in kiel <tld/>on por eniri
        <fnt>
          <aut>Henri Vallienne</aut>
          <vrk><url ref="&Tekstaro;?nomo=kastelo-de-prelongo">Kastelo de Prelongo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_l;&c_a;&c_v;&c_a;&c_ch;&c_k;&c_a;</trd>,
      <trd>&c_u;&c_s;&c_l;&c_o;&c_n;&c_ch;&c_y;&c_k;</trd>,
      <trd>&c_z;&c_e;&c_d;&c_l;&c_ib;&c_ch;&c_a;&c_k;</trd>
    </trdgrp>
    <trd lng="de">Fu&szlig;bank</trd>
    <trd lng="en">footstool</trd>
    <trd lng="fr">pouf</trd>
    <trdgrp lng="pl">
      <trd>podn&oacute;&zdot;ek</trd>,
      <trd>sto&lstroke;ek</trd>
    </trdgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>piedripozilo</ke>, <ke>piedse&#285;o</ke>, <ke>piedapogilo</ke>.
  </rim>

  <trdgrp lng="uk">
    <trd>&#1087;&c_ib;&c_d;&#1085;&c_ib;&c_zh;&#1086;&#1082;</trd>,
    <trd>&c_s;&c_t;&c_ib;&#1083;&#1100;&c_ch;&c_i;&#1082; &#1087;&c_ib;&c_d; &#1085;&#1086;&c_g;&c_i;</trd>,
    <trd>&c_s;&c_t;&c_ib;&#1083;&#1100;&c_ch;&c_i;&#1082;</trd>,
    <trd>&#1086;&c_s;&#1083;&c_ib;&#1085;&c_ch;&c_i;&#1082;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#33050;&#20979; <pr>ji&#462;od&#232;ng</pr></trd>,
    <trd>&#33139;&#20979; <pr>ji&#462;od&#232;ng</pr></trd>
  </trdgrp>
</drv>
  </art>

