  <trd lng="be">&c_g;&c_ib;&c_p;&c_s;&c_t;&c_a;&c_r;</trd>
  <trd lng="en">hipster</trd>
  <trd lng="fr">hipster</trd>
  <trd lng="zh">&#x5C0F;&#x6E05;&#x65B0; <pr>xi&#x1CE;oq&#x12B;ngx&#x12B;n</pr></trd>
