<vortaro>
<art mrk="$Id: krasul.xml,v 1.16 2018/09/19 22:10:21 revo Exp $">
<kap>
  <rad>krasul</rad>/o
</kap>
<drv mrk="krasul.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref>
      el familio <ref tip="malprt" cel="krasul1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Crassula</trd>), kun bone odorantaj floroj kaj
      dikaj
      folioj:
      <ekz>
        <tld/>o<klr>[j ... kreskas]</klr> en suda Afriko
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref="https://eo.wikipedia.org/?curid=551315">Krasulo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Dickblatt</trd>
  <trd lng="fr">crassula</trd>
  <trd lng="hu">pozsga</trd>
  <trd lng="nl">crassula<klr tip="ind"> (vetplant)</klr></trd>
  <trd lng="pl">grubosz</trd>
  <trd lng="pt">cr&aacute;ssula</trd>
  <trd lng="ro">crassula</trd>
  <trd lng="ru">&c_t;&c_o;&c_l;&c_s;&c_t;&c_ja;&c_n;&c_k;&c_a;</trd>
</drv>
</art>
