<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>krasul</rad>/o
</kap>
<drv mrk="krasul.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="krasul.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Crassula</trd>), kun bonodorantaj floroj kaj dikaj folioj.
    </dif>
  </snc>
  <trd lng="de">Dickblatt</trd>
</drv>
<drv mrk="krasul.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Crassulaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Dickblattgew&auml;chse</trd>
</drv>
</art>
