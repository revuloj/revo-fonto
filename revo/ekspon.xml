<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ekspon</rad>/i
</kap>
<drv mrk="ekspon.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ekspon.0i.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      Me&hcirc;anike elmeti plakon a&ubreve; alian impresi&gcirc;eman substancon al la luma
      efiko:
      <ekz>
        Connie tuj ekploris je la supozo, ke oni malfermos &scirc;ian fotilon
        kaj <tld/>os &scirc;ian filmon
        <fnt>
        <aut>D. Harlow</aut>,
        <vrk>Re: Lingvo-Kverelo &ccirc;e Norda Konsilantaro</vrk>,
        <lok>soc.culture.esperanto, 2001-11-29</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_e;&c_k;&c_s;&c_p;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_p;&c_a;&c_d;&c_v;&c_ja;&c_r;&c_g;&c_a;&c_c;&c_mol;
    &c_d;&c_z;&c_je;&c_ja;&c_n;&c_mol;&c_n;&c_ju; &c_s;&c_mol;&c_v;&c_ja;&c_t;&c_l;&c_a;)</klr></trd>
    <trdgrp lng="de">
      <trd>aussetzen</trd>,
      <trd>belichten</trd>,
      <trd>exponieren</trd>
    </trdgrp>
    <trd lng="fr">exposer<klr tip="ind"> (phot.)</klr></trd>
    <trdgrp lng="hu">
      <trd>megvil&aacute;g&iacute;t</trd>,
      <trd>expon&aacute;l</trd>,
    </trdgrp>
    <trd lng="ru">&c_e;&c_k;&c_s;&c_p;&c_o;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="ekspon.0i.klarigi">
    <dif>
      Prezenti klare kaj detale iun temon por ta&ubreve;ge komprenigi &gcirc;in:
      <ekz>
        <tld/>o de la motivoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la ununura persono kiu kura&gcirc;is <tld/>i siajn ideojn en nia forumo
        <fnt>
        <aut>G. Riudor</aut>,
        <vrk>Unui&gcirc;o: Alain kaj Nikolao</vrk>,
        <lok>soc.culture.esperanto, 1999-02-18</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_e;&c_z;&c_e;&c_n;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>darstellen</trd>
    </trdgrp>
    <trd lng="fr">exposer<klr tip="ind"> (&eacute;noncer)</klr></trd>
    <trdgrp lng="hu">
      <trd>megvil&aacute;g&iacute;t<klr tip="ind"> (t&eacute;m&aacute;t)</klr></trd>,
      <trd>kifejt</trd>,
    </trdgrp>
    <trd lng="ru">&c_i;&c_z;&c_l;&c_a;&c_g;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="ekspon.0i.EKON">
    <dif>
      Ekspozicii varojn.
    </dif>
    <refgrp tip="vid">
      <ref cel="deklam.ekspon0i">ekspondeklami</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_s;&c_t;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>,
      <trd>&c_e;&c_k;&c_s;&c_p;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_t;&c_a;&c_v;&c_a;&c_r;&c_y;)</klr></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>ausstellen</trd>,
      <trd>auslegen</trd>
    </trdgrp>
  </snc>
  <trd lng="es">exponer</trd>
  <trd lng="hu">ki&aacute;ll&iacute;t</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_e;&c_k;&c_s;&c_p;&c_o;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;<klr tip="amb"> (&c_t;&c_o;&c_v;&c_a;&c_r;&c_y;)</klr></trd>
  </trdgrp>
</drv>
</art>
