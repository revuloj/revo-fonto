<art mrk="$Id: semafo.xml,v 1.25 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>semafor</rad>/o
</kap>
<drv mrk="semafo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FER</uzo>
  <uzo tip="fak">TRA</uzo>
  <snc>
    <dif>
      Aparato starigita sur marbordoj, &ccirc;e fervojo a&ubreve; en
      trafikplenaj lokoj
      por dissendi optikajn signalojn:
      <ekz>
        per tia sistemo same eblus &scirc;alti iujn signallampojn por la
        piloto,
        kvaza&ubreve; trafikan <tld/>on
        <fnt><aut>Pejno Simono</aut>:
        <vrk><url ref="&amp;Monato/007219.html">Prefere pakete</url></vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 02/2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_e;&c_m;&c_a;&c_f;&c_o;&c_r;</trd>,
    <trd>&c_s;&c_mol;&c_v;&c_je;&c_t;&c_l;&c_a;&c_f;&c_o;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_s;&c_v;&c_je;&c_t;&c_o;&c_f;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="ca">sem&agrave;for</trd>
  <trdgrp lng="cs">
    <trd>semafor</trd>,
    <trd>svět. návěstidlo</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Signal</trd>,
    <trd>Ampel</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>semaphore</trd>,
    <trd>signal</trd>
  </trdgrp>
  <trd lng="es">sem&aacute;foro</trd>
  <trdgrp lng="fr">
    <trd>feu de signalisation</trd>,
    <trd>s&eacute;maphore</trd>
  </trdgrp>
  <trd lng="hr">semafor</trd>
  <trdgrp lng="hu">
    <trd>szemafor</trd>,
    <trd>jelz&odblac;l&aacute;mpa</trd>,
    <trd>villanyrend&odblac;r</trd>
  </trdgrp>
  <trd lng="it">semaforo</trd>
  <trdgrp lng="nl">
    <trd>signaal</trd>,
    <trd>semafoor</trd>,
    <trd>verkeerslicht</trd>
  </trdgrp>
  <trd lng="pl">semafor</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_m;&c_a;&c_f;&c_o;&c_r;</trd>,
    <trd>&c_s;&c_v;&c_je;&c_t;&c_o;&c_f;&c_o;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>optické návestidlo</trd>,
    <trd>semafor</trd>
  </trdgrp>
  <trd lng="sv">trafiksignal</trd>
</drv>
</art>
