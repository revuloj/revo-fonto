    <trd>semafor</trd>,
    <trd>sv&#x11B;t. n&#xE1;v&#x11B;stidlo</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Signal</trd>,
    <trd>Ampel</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>semaphore</trd>,
    <trd>signal</trd>
  </trdgrp>
  <trd lng="es">sem&aacute;foro</trd>
