<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: labil.xml,v 1.2 2023/09/01 19:10:57 afido Exp $">
    <kap>
      <rad>labil</rad>/a
    </kap>

<drv mrk="labil.0a">
  <kap><tld/>a</kap>
  <snc mrk="labil.0a.malstabila">
    <dif>
      <ref tip="dif" cel="stabil.mal0a">Malstabila</ref>;
      facile perdanta sian ekvilibron, bonstaton:
      <ekz>
        homo kun <tld/>a sano
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 2 (100)</lok>
        </fnt>;
      </ekz>
      <ekz>
        patologia <tld/>eco
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 5 (103)</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam kaj plezuro kaj doloro okazas en la psiko temas pri <tld/>aj emocioj
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Na&ubreve;</lok>
        </fnt>;
      </ekz>
      <ekz>
        en tiu strange <tld/>a animstato, kiam la spirito &scirc;vebas inter maldormo kaj
        ekson&gcirc;o, en mia ekscitita fantazio riveli&gcirc;is malpudoraj seksludoj
        <fnt>
          <bib>SkandalJozef</bib>
          <lok>Plendoj de Jozefo arestita</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="anim.egal0a">egalanima</ref>,
      <ref cel="firm.0a">firma</ref>,
      <ref cel="konsta.0a.anima">konstanta<sncref/></ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="sxance.0igxi">&scirc;anceli&gcirc;i</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>labil</trd>,
      <trd>unbest&auml;ndig</trd>
    </trdgrp>
    <trd lng="en">unstable</trd>
    <trd lng="fr">instable</trd>
  </snc>
  <trd lng="ja">&#x4E0D;&#x5B89;&#x5B9A;&#x306A;  [<ind>&#x3075;&#x3042;&#x3093;&#x3066;&#x3044;&#x306A;</ind>]</trd>
</drv>

  </art>

