<art mrk="$Id: rijet.xml,v 1.13 2018/11/21 21:10:15 revo Exp $">
<kap>
  <rad>rijet</rad>/o
</kap>
<drv mrk="rijet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="mangx.0ajxo">Man&gcirc;a&jcirc;o</ref>, 
      konsistanta el hakita porka&jcirc;o a&ubreve; kokida&jcirc;o, miksita
      kun graso, kutime man&gcirc;ata &scirc;mirita sur pantran&ccirc;a&jcirc;o:
      <ekz>
        Manso estas fama pro sia fama Katedralo Sankta Juliano, kaj anka&ubreve; pro la <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=27693"
              >Le Mans</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/f/fb/Rillettes_de_canard.jpg/435px-Rillettes_de_canard.jpg"
      prm="CC BY 2.0"
      >anasa <tld/>o
        <fnt>
          <aut>syvwlch, https://www.flickr.com/photos/27164521@N00/4918340214/</aut>
          <lok><url ref="&WCW;/File:Rillettes_de_canard.jpg"
              >https://commons.wikimedia.org/wiki/File:Rillettes_de_canard.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="cs">
    <trd>fa&scaron;&iacute;rka</trd>,
    <trd>karban&aacute;tek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Rillettes</trd>,
    <trd>Pottsuse</trd>
  </trdgrp>
  <trd lng="fr">rillette</trd>
  <trd lng="hu">p&aacute;st&eacute;tom</trd>
  <trd lng="nl">pastei</trd>
  <trd lng="pt">picado de porco ou de galinha</trd>
  <trd lng="ru">&c_r;&c_i;&c_je;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>fa&scaron;&iacute;rka</trd>,
    <trd>karbon&aacute;tka</trd>
  </trdgrp>
</drv>
</art>
