<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: hxlamidi.xml,v 1.4 2025/11/22 02:05:47 afido Exp $">
    <kap>
      <rad>&hcirc;lamidi</rad>/o, <var><kap><rad var="k">klamidi</rad>o</kap></var>
    </kap>

<drv mrk="hxlamidi.0o">
  <kap><tld/>o, <var><kap><tld var="k"/>o</kap></var></kap>
  <uzo tip="fak">BAK</uzo>
  <snc>
    <dif>
      Iu el tre malgrandaj gramnegativaj <ref tip="super" cel="bakter.0o">bakterio</ref>j el la
      genro <trd lng="la">Chlamydia</trd> parazitantaj bestajn &ccirc;elojn:
      <ekz>
        krom la penicilino anka&ubreve; cikloserino kaj tetraciklinoj estas efikaj kontra&ubreve; la
        <tld/>oj
        <fnt>
          <vrk><url
          ref="http://www.literatura.bucek.name/brno/fischer_hxlamidio.pdf">Esperanto-Brno</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        sciencistoj estis tiam konvinkitaj, ke &ccirc;iuj agantoj ka&ubreve;zantaj infektajn
        malsanojn (bakterioj, <tld var="k"/>oj, protozooj, virusoj ktp) devas enhavi DNA
        kaj/a&ubreve; RNA
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="https://www.monato.be/2006/008861.php">Nova etapo en biologio kaj
          medicino</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_h;&c_l;&c_ja;&c_m;&c_ib;&c_d;&c_y;&c_ja;</trd>
    <trd lng="de">Chlamydie</trd>
    <trd lng="pl">chlamydie <klr>(bakterie)</klr></trd>
  </snc>
  <trdgrp lng="zh">
    <trd>&#34915;&#x7D30;&#x83CC; <pr>y&#x12B;x&#xEC;j&#x16B;n</pr></trd>,
    <trd>&#x975E;&#x6DCB; <pr>f&emacron;il&#xEC;n</pr></trd>
  </trdgrp>
</drv>
</art>

