<kap>
  <rad>krucifer</rad>/oj
</kap>
<drv mrk="krucif.0oj">
  <kap>
    <tld/>oj
  </kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      Tradicia nomo de <ref tip="dif" cel="brasik1.0oj">brasikacoj</ref>:
      <ekz>
        <tld/>oj a&ubreve; brasikacoj estas planto-familio en kiu membras,
        inter aliaj, brasiko, kardamino, kreno, rafaneto, sinapo, kaj
        vasabio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18072">Kruciferoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_y;&c_zh;&c_a;&c_k;&c_v;&c_je;&c_t;&c_n;&c_y;&c_ja;</trd>,
    <trd>&c_k;&c_r;&c_y;&c_zh;&c_y;&c_k;&c_a;&c_v;&c_y;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>cruc&iacute;feres</trd>,
    <trd>brassic&agrave;cies</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kreuzbl&uuml;tler</trd>,
    <trd>Kreuzbl&uuml;tengew&auml;chse</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>crucifers</trd>,
    <trd>mustards</trd>,
    <trd>cabbage family</trd>,
    <trd>Cruciferae</trd>
  </trdgrp>
  <trd lng="fa">&f_ch;&a_l;&f_y;&f_p;&a_A;&f_y;&f_y;&a_A;&a_n;</trd>
  <trd lng="fr">crucif&egrave;res</trd>
  <trdgrp lng="hu">
    <trd>keresztesek</trd>,
    <trd>keresztesvir&aacute;g&uacute;ak</trd>
  </trdgrp>
  <trd lng="ja">&#x5341;&#x5B57;&#x82B1;&#x79D1;  <pr>&#x3058;&#x3085;&#x3046;&#x3058;&#x304B;&#x304B;</pr></trd>
  <trd lng="pl">kapustowate</trd>
  <trdgrp lng="pt">
    <trd>cruc&iacute;feras</trd>,
    <trd>brassic&aacute;ceas</trd>
  </trdgrp>
  <trd lng="ro">brassicaceae</trd>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_s;&c_t;&c_o;&c_c;&c_v;&c_je;&c_t;&c_n;&c_y;&c_je;</trd>
  <trd lng="uk">&c_h;&c_r;&c_je;&c_s;&c_t;&c_o;&c_c;&c_v;&c_ib;&c_t;&c_ib;</trd>
  <trd lng="zh">&#x5341;&#x5B57;&#x82B1;&#x79D1; <pr>sh&#xED;z&#xEC;hu&#x101;k&#x113;</pr></trd>
</drv>
