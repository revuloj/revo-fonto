<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: impert.xml,v 1.24 2023/11/19 10:22:12 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>impertinent</rad>/a
</kap>
<drv mrk="impert.0a">
  <kap><tld/>a</kap>
  <snc mrk="impert.0a.PSI">
    <dif>
      Maldece a&ubreve; arogante <ref tip="super" cel="respek.mal0a.PSI">malrespekta</ref>:
      <ekz>
        <tld/>a homo, konduto, letero, parolo, tono, rigardo;
      </ekz>
      <ekz>
        ne havi sufi&ccirc;e da inteligento por konvene paroli,
        nek sufi&ccirc;e da prudento por silenti, jen la ka&ubreve;zo de &ccirc;ia
        <tld/>eco;
      </ekz>
      <ekz>
        li malsereni&gcirc;is &ccirc;iam pli, mordis la ungojn kaj altabli&gcirc;is kun
        <tld/>a mieno
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gxenti.mal0a">mal&gcirc;entila</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_h;&c_a;&c_b;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>dot&#x11B;rn&#xFD;</trd>,
    <trd>drz&#xFD;</trd>,
    <trd>hubat&#xFD;</trd>,
    <trd>impertinentn&#xED;</trd>,
    <trd>neomalen&#xFD;</trd>,
    <trd>nestoudn&#xFD;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>impertinent</trd>,
    <trd>frech</trd>,
    <trd>unversch&auml;mt</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>impertinent</trd>,
    <trd>cheeky</trd>,
    <trd>impudent</trd>,
    <trd>insolent</trd>
  </trdgrp>
  <trd lng="es">impertinente</trd>
  <trd lng="fr">impertinent<klr tip="amb"> (adj.)</klr></trd>
  <trd lng="hu">szemtelen</trd>
  <trdgrp lng="ja">
    <trd>&#x7121;&#x793C;&#x306A;  <pr>&#x3076;&#x308C;&#x3044;&#x306A;</pr></trd>,
    <trd>&#x539A;&#x304B;&#x307E;&#x3057;&#x3044;  <pr>&#x3042;&#x3064;&#x304B;&#x307E;&#x3057;&#x3044;</pr></trd>,
    <trd>&#x751F;&#x610F;&#x6C17;&#x306A;  <pr>&#x306A;&#x307E;&#x3044;&#x304D;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>impertinent</trd>,
    <trd>onbeschaamd</trd>,
    <trd>brutaal</trd>,
    <trd>arrogant</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>impertinente<klr tip="ind"> (adj.)</klr></trd>,
   <trd>insolente<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp><trd lng="pt">impertinente<klr tip="amb"> (adj.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_g;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_a;&c_h;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bezo&#x10D;iv&#xFD;</trd>,
    <trd>impertinentn&#xFD;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x437;&#x443;&#x445;&#x432;&#x430;&#x43B;&#x438;&#x439;</trd>,
    <trd>&#x43D;&#x430;&#x445;&#x430;&#x431;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x76AE;&#x8138; <pr>p&#xED;li&#x1CE;n</pr></trd>,
    <trd>&#x6C92;&#x5927;&#x6C92;&#x5C0F; <pr>m&#xE9;id&#xE0;m&#xE9;ixi&#x1CE;o</pr></trd>,
    <trd>&#x7C97;&#x91CE; <pr>c&#x16B;y&#x11B;</pr></trd>,
    <trd>&#x60EB;&#x8D56; <pr>b&#xE8;il&#xE0;i</pr></trd>,
    <trd>&#x5F20;&#x72C2; <pr>zh&#x101;ngku&#xE1;ng</pr></trd>,
    <trd>&#x4E0D;&#x900A; <pr>b&#xF9;x&#xF9;n</pr></trd>,
    <trd>&#x4FAE;&#x8FB1;&#x6027; <pr>w&#x1D4;r&#xF9;x&#xEC;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="impert.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="impert.0ajxo.MOR">
    <dif>
      Spite sen&gcirc;ena ago, senhonta&jcirc;o:
      <ekz>
        la manon disrompis al mi via &ccirc;uk&ccirc;a amika&ccirc;o, kaj li jam perdis
        siajn okulojn pro la <tld/>a&jcirc;o
        <fnt><bib>&Ccirc;uk&ccirc;oj</bib>, <lok>la trimova &scirc;akproblemo, (tria
        skizo
        el la &ccirc;uk&ccirc;a vivo), 7, la nesolvebla problemo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_h;&c_a;&c_b;&c_s;&c_t;&c_v;&c_a;</trd>
  <trd lng="de">Frechheit</trd>
  <trdgrp lng="en">
    <trd>impertinence</trd>,
    <trd>cheekiness</trd>,
    <trd>impudence</trd>,
    <trd>insolence</trd>
  </trdgrp>
  <trd lng="fr">impertinence<klr tip="amb"> (acte)</klr></trd>
  <trdgrp lng="nl">
    <trd>brutaliteit</trd>,
    <trd>iets <ind>impertinent</ind>s</trd>
  </trdgrp>
  <trd lng="pt">impertin&ecirc;ncia<klr tip="amb"> (ato, a&ccedil;&atilde;o)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_g;&c_l;&c_o;&c_s;&c_t;&c_mol;<klr tip="ind"> (&c_p;&c_o;&c_s;&c_t;&c_u;&c_p;&c_o;&c_k;)</klr></trd>,
    <trd>&c_n;&c_a;&c_h;&c_a;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;<klr tip="ind">
    (&c_p;&c_o;&c_s;&c_t;&c_u;&c_p;&c_o;&c_k;)</klr></trd>,
    <trd>&c_h;&c_a;&c_m;&c_s;&c_t;&c_v;&c_o;<klr tip="amb"> (&c_p;&c_o;&c_s;&c_t;&c_u;&c_p;&c_o;&c_k;)</klr></trd>
  </trdgrp>
  <trd lng="zh">&#x72C2;&#x5984; <pr>ku&#xE1;ngw&#xE0;ng</pr></trd>
</drv>

<drv mrk="impert.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="impert.0ulo.MOR">
    <dif>
      Konscie maldeca, aroganta homo sen respekto al aliaj:
      <ekz>
        kiu vi estas, <tld/>ulo? Maldelikata vojorabisto
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 9a, aventura
        voja&gcirc;o</lok></fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_h;&c_a;&c_b;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>dot&#x11B;ra</trd>,
    <trd>drzoun</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cheeky rascal</trd>,
    <trd>saucy fellow</trd>
  </trdgrp>
  <trd lng="fr">impertinent<klr tip="amb"> (personne)</klr></trd>
  <trd lng="ja">&#x7121;&#x793C;&#x8005;  <pr>&#x3076;&#x308C;&#x3044;&#x3082;&#x306E;</pr></trd>
  <trdgrp lng="nl">
    <trd>impertinent iemand</trd>,
    <trd>onbeschofterik</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>impertinente<klr tip="amb"> (pessoa)</klr></trd>,
   <trd>insolente<klr tip="amb"> (pessoa)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_g;&c_l;&c_je;&c_c;</trd>,
    <trd>&c_n;&c_a;&c_h;&c_a;&c_l;</trd>,
    <trd>&c_h;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dotieravec</trd>,
    <trd>drz&#xE1;&#x148;</trd>
  </trdgrp>
  <trd lng="uk">&#x43D;&#x430;&#x445;&#x430;&#x431;&#x430;</trd>
</drv>
</art>
