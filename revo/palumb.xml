<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: palumb.xml,v 1.20 2024/02/16 19:24:10 revo Exp $">
<kap>
  <rad>palumb</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="palumb.0o">
  <kap><tld/>o</kap>
  <snc mrk="palumb.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="kolomb.ringo0o">Ringokolombo</ref>:
      <ekz>
        ie <tld/>oj rukulis, odoro de bovinoj
        &scirc;vebis en la aero
        <fnt><aut>I. Nemere</aut>, <vrk>Sur Kampo Granita</vrk>, 
        <lok>1983</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>o havas rozkoloran bruston kaj verdajn nuancoj en la strioj super la
        kolmakuloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17708">Palumbo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_h;&c_ib;&c_r;</trd>
  <trd lng="bg">&c_g;&c_r;&c_i;&c_v;&c_ja;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>h&rcaron;ivn&aacute;&ccaron; obecn&yacute;</trd>,
    <trd>holub h&rcaron;ivn&aacute;&ccaron;</trd>
  </trdgrp>
  <trd lng="de">Ringeltaube</trd>
  <trd lng="en">wood-pigeon</trd>
  <trd lng="fa">&f_k;&a_b;&a_w;&a_t;&a_r; &a_j;&a_n;&f_g;&a_l;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>palombe</trd>,
    <trd>pigeon ramier</trd>,
    <trd>ramier</trd>
  </trdgrp>
  <trd lng="hu">&ouml;rv&ouml;s <ind>galamb</ind></trd>
  <trd lng="ja">&#x30E2;&#x30EA;&#x30D0;&#x30C8;</trd>
  <trd lng="nl">houtduif</trd>
  <trd lng="pl">grzywacz</trd>
  <trdgrp lng="pt">
    <trd>asa-branca</trd>,
    <trd>jaca&ccedil;u</trd>,
    <trd>pomba-trocaz</trd>,        
    <trd>pomba-carij&oacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_ja;&c_h;&c_i;&c_r;&c_mol;</trd>,
    <trd>&c_v;&c_i;&c_t;&c_ju;&c_t;&c_je;&c_n;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hrivn&aacute;k</trd>,
    <trd>hrivn&aacute;&ccaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x433;&#x43E;&#x43B;&#x443;&#x431; &#x43B;&#x456;&#x441;&#x43E;&#x432;&#x438;&#x439;</trd>,
    <trd>&#x43F;&#x440;&#x438;&#x43F;&#x443;&#x442;&#x435;&#x43D;&#x44C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6591;&#x5C3E;&#x6797;&#x9D3F; <pr>b&#x101;nw&#x11B;il&#xED;ng&#x113;</pr></trd>,
    <trd>&#x6591;&#x5C3E;&#x6797;&#x9E3D; <pr>b&#x101;nw&#x11B;il&#xED;ng&#x113;</pr></trd>
  </trdgrp>
</drv>
</art>
