<vortaro>
<art mrk="$Id$">
<kap><rad>palumb</rad>/o</kap>
<drv mrk="palumb.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <ref tip="dif" cel="kolomb.ringo0o">Ringokolombo</ref>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_h;&c_ib;&c_r;</trd>
  <trd lng="de">Ringeltaube</trd>
  <trd lng="hu">&ouml;rv&ouml;s <ind>galamb</ind></trd>
  <trd lng="nl">houtduif</trd>
  <trd lng="pl">grzywacz</trd>
  <trdgrp lng="pt">
    <trd>asa-branca</trd>,
    <trd>jaca&ccedil;u</trd>,
    <trd>pomba-trocaz</trd>,        
    <trd>pomba-carij&oacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_ja;&c_h;&c_i;&c_r;&c_mol;</trd>,
    <trd>&c_v;&c_i;&c_t;&c_ju;&c_t;&c_je;&c_n;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
