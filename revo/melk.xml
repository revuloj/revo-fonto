<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: melk.xml,v 1.27 2025/11/23 18:39:09 revo Exp $">
  <kap>
  <ofc>*</ofc>
  <rad>melk</rad>/i
  <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="melk.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Eltiri per premado <ref cel="lakt.0o.mambesto">lakto</ref>n el mamoj de besto:
      <ekz>
        <tld/>i &scirc;afinon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> helpas lin <tld/>i la bovinojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ni ne <tld/>as aliajn Esperanto-organizojn per 30 procentoj, e&ccirc; inverse, multaj
        el niaj Esperantaj servoj estas senpagaj
        <fnt>
          <bib>Kon11</bib>
          <lok>Intervjuo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[mi]</klr> apena&ubreve; eliris escepte por <tld/>i mian kaprinaron
        <fnt>
          <bib>Rob</bib>
          <lok>Parto III</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i kaproviron
        <klr>(vane peni, provi neebla&jcirc;on)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
   </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="br">goro</trd>
  <trd lng="cs">dojiti <klr>(ml&eacute;ko)</klr></trd>
  <trd lng="de">melken</trd>
  <trd lng="en">milk</trd>
  <trd lng="es">orde&ntilde;ar</trd>
  <trd lng="fr">traire</trd>
  <trd lng="hu">fej</trd>
  <trdgrp lng="ja">
    <trd>&#x4E73;&#x3092;&#x643E;&#x308B;  <pr>&#x3061;&#x3061;&#x3092;&#x3057;&#x307C;&#x308B;</pr></trd>,
    <trd>&#x643E;&#x308A;&#x53D6;&#x308B;  <pr>&#x3057;&#x307C;&#x308A;&#x3068;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">melken</trd>
  <trd lng="pl">doi&cacute;</trd>
  <trdgrp lng="pt">
    <trd>ordenhar</trd>,
    <trd>mungir</trd>,
    <trd>tirar <ind>leite</ind> a</trd>
  </trdgrp>
  <trd lng="ro">mulge</trd>
  <trd lng="ru">&c_d;&c_o;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">doji&tcaron; <klr>(mlieko)</klr></trd>
  <trd lng="sv">mj&ouml;lka</trd>
  <trd lng="uk">&c_d;&c_o;&c_ji;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x6324;&#x5976; <pr>j&#x1D0;n&#x1CE;i</pr></trd>,
    <trd>&#x64E0;&#x5976; <pr>j&#x1D0;n&#x1CE;i</pr></trd>,
    <trd>&#x5B2D; <pr>n&#x1CE;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="melk.0ebla">
  <kap><tld/>ebla</kap>
  <snc>
    <dif>
      (ordinare pri hejmbestoj) Produktanta lakton:
      <ekz>
        <tld/>ebla bovino, &ccirc;evalino, kamelino;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la pastroj transformis kristanismon al
        senfine <ind><tld/>ebla bovino</ind>
        <klr>(senmezure ekspluatebla afero)</klr>
        <trd lng="de">milchende Kuh</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_o;&c_j;&c_n;&c_y;</trd>
  <trdgrp lng="de">
    <trd>milchgebend</trd>,
    <trd>milchend</trd>
  </trdgrp>
  <trd lng="pl">dojna</trd>
  <trd lng="ro">de muls</trd>
  <trd lng="ru">&c_d;&c_o;&c_j;&c_n;&c_y;&c_j;</trd>
</drv>

</art>
