<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>Kepler</rad>/o
</kap>
<drv mrk="kepler.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Germanlingve: Johannes Kepler, 1571-1630. Germana astronomo.
    </dif>
  </snc>
  <trd lng="fr">Kepler</trd>
</drv>
<drv mrk="kepler.0a">
  <kap><tld lit="k"/>a</kap>
  <snc>
    <dif>
      Iel rilatanta kun la malkovroj de <tld/>o:
      <ekz>
        <tld lit="k"/>a astronomio;
      </ekz>
      <ekz>
        la tri <tld lit="k"/>aj le&gcirc;oj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="elips.0o.linio">elipso</ref>,
      <ref cel="radius.0vektoro">radiusvektoro</ref>,
      <ref cel="aks.grandaduon0o">granda duonakso</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>k&eacute;pl&eacute;rien</trd>,
    <trd>de <ind>Kepler</ind></trd>
  </trdgrp>
</drv>
</art>

