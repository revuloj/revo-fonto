<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Kepler</rad>/o
</kap>
<drv mrk="kepler.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <dif>
      Germanlingve: Johannes <trd lng="de">Kepler</trd>, 1571-1630.
      Germana astronomo.
    </dif>
  </snc>
  <trd lng="be">&c_K;&c_je;&c_p;&c_l;&c_je;&c_r;</trd>
  <trd lng="en">Kepler</trd>
  <trd lng="fr">Kepler</trd>
  <trd lng="nl">Johannes <ind>Kepler</ind></trd>
  <trd lng="hu">Kepler</trd>
  <trd lng="it">Keplero</trd>
  <trd lng="pl">Kepler</trd>
  <trd lng="ru">&c_K;&c_je;&c_p;&c_l;&c_je;&c_r;</trd>
</drv>

<drv mrk="kepler.k0a">
  <kap><tld lit="k"/>a</kap>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <dif>
      Iel rilatanta kun la malkovroj de <tld/>o:
      <ekz>
        <tld lit="k"/>a astronomio;
      </ekz>
      <ekz>
        la tri <ref cel="legx.kepleraj0oj"><tld lit="k"/>aj
        le&gcirc;oj</ref>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_K;&c_je;&c_p;&c_l;&c_je;&c_r;&c_a;</trd>,
    <trd>&c_k;&c_je;&c_p;&c_l;&c_je;&c_r;&c_a;&c_w;&c_s;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>von <ind>Kepler</ind></trd>,
    <trd>Keplers</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Kepler['s]</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>de <ind>Kepler</ind></trd>,
    <trd>k&eacute;pl&eacute;rien</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kepleri</trd>,
    <trd>Kepler-f&eacute;le</trd>
  </trdgrp>
  <trd lng="it">kepleriano</trd>
  <trd lng="nl">kepler-</trd>
  <trdgrp lng="ru">
    <trd>&c_K;&c_je;&c_p;&c_l;&c_je;&c_r;</trd>,
    <trd>&c_k;&c_je;&c_p;&c_l;&c_je;&c_r;&c_o;&c_v;&c_s;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>

