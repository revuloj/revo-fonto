<art mrk="$Id: malik.xml,v 1.6 2022/05/01 12:05:12 revo Exp $">
<kap>
  <rad>Malik</rad>/o
</kap>
<drv mrk="malik.0o">
  <kap><tld/>o, <var><kap><tld/></kap></var></kap>
  <snc mrk="malik.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> arabdevena:
      <ekz>
        kion pensas la patro de <tld/>o en 2009 pri la nuna situacio en 
        &Ccirc;inio?
        <fnt>
          <aut>O. Kerziouk</aut>,
          <vrk><url
          ref="http://groups.yahoo.com/group/per-esperanto-politiko/message/8829">
          Temoj por debato</url></vrk>,
          <lok>2009-07-09</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/> Zidi <klr>...</klr> ludis rolon de tajloro-lernanto
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Malik_Zidi">
          Malik Zidi</url>, 2010-07-28</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">Malik</trd>
  <trd lng="fr">Malik</trd>
  <trd lng="pl">Malik</trd>     
  <trd lng="ro">Malik</trd>
</drv>
</art>

