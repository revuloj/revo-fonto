<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kastr.xml,v 1.19 2022/03/21 17:51:57 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kastr</rad>/i
</kap>
<drv mrk="kastr.0i">
  <kap>
    <ofc>*</ofc><tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Senigi homon a&ubreve; beston je generpovo, fortran&ccirc;ante la <ref tip="vid" cel="gonad.0o.ANA">gonadojn</ref> a&ubreve; ligante ties
      sekreciajn kanalojn:
      <ekz>
        la <ind><tld/>ito</ind>j soprane kantas<fnt>B</fnt>.       
        <trd lng="br">spazhard</trd>
        <trd lng="fr">castrat</trd>
        <trd lng="io">kastrito</trd>
        <trdgrp lng="nl">
          <trd>castraat</trd>,
          <trd>gesnedene</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="eks.0.kastro">eks-</ref>,
    <ref cel="euxnuk.0o">e&ubreve;nuko</ref>
  </refgrp>
  <trd lng="be">&c_s;&c_p;&c_a;&c_k;&c_l;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>
  <trd lng="br">spazha&ntilde;</trd>
  <trd lng="ca">castrar</trd>
  <trdgrp lng="de">
     <trd>kastrieren</trd>,
     <trd>verschneiden</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>castrate</trd>,
     <trd>emasculate</trd>,
     <trd>geld</trd>,
     <trd>spay</trd>,
     <trd>neuter</trd>,
     <trd>sterilize</trd>
  </trdgrp>
  <trd lng="es">castrar</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_kh;&a_t;&a_h; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_ayn;&a_q;&f_y;&a_m; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>castrer</trd>,
    <trd>ch&acirc;trer</trd>,
    <trd>couper<klr tip="ind"> (castrer)</klr></trd>,
    <trd>&eacute;masculer</trd>
  </trdgrp>
  <trd lng="fy">kastrearje</trd>
  <trdgrp lng="hu">
     <trd>kiher&eacute;l</trd>,
     <trd>misk&aacute;rol</trd>,
     <trd>ivartalan&iacute;t</trd>,
     <trd>kasztr&aacute;l</trd>
  </trdgrp>
  <trd lng="io">kastrar</trd>
  <trdgrp lng="nl">
    <trd>castreren</trd>,
    <trd>ontmannen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kastrowa&cacute;</trd>,
    <trd>trzebi&cacute;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>castra</trd>,
    <trd>extermina</trd>
  </trdgrp>   
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_s;&c_t;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_s;&c_k;&c_o;&c_p;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x430;&#x441;&#x442;&#x440;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43E;&#x441;&#x43A;&#x43E;&#x43F;&#x43B;&#x44F;&#x442;&#x438;</trd>
  </trdgrp>
  <trd lng="vo">negen&uuml;k&ouml;n</trd>
</drv>
</art>
