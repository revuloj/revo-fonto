<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kastr</rad>/i
</kap>
<drv mrk="kastr.0i">
  <kap>
    <ofc>*</ofc><tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Senigi homon a&ubreve; beston je naskigpovo, fortran&ccirc;ante la seksajn
      glandojn:
      <ekz>
        la <tld/>itoj soprane kantas<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>kastrieren</trd>,
     <trd>verschneiden</trd>
  </trdgrp>
  <trd lng="en">castrate</trd>
  <trd lng="nl">castreren</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_s;&c_t;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_s;&c_k;&c_o;&c_p;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
