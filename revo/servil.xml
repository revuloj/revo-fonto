    <trd>unterw&uuml;rfig</trd>
  </trdgrp>
  <trd lng="en">servile</trd>
  <trd lng="es">obsequioso</trd>
  <trdgrp lng="fa">
    <trd>&a_n;&a_w;&f_k;&a_r;&a_S;&a_f;&a_t;&a_A;&a_n;&a_h;</trd>,
    <trd>&a_b;&a_r;&a_d;&a_h;&#8204;&a_w;&a_A;&a_r;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>obs&eacute;quieux</trd>,
    <trd>servile<klr> (obs&eacute;quieux)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szolgai</trd>,
    <trd>szolgalelk&udblac;</trd>,
    <trd>szervilis</trd>
  </trdgrp>
  <trd lng="it">servile</trd>
  <trdgrp lng="pl">
    <trd>s&lstroke;u&zdot;alczy</trd>,
    <trd>czo&lstroke;obitny</trd>
