<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: servil.xml,v 1.8 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>servil</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="servil.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="humil.0a.iu">Humila&ccirc;a</ref>, tro flatema
      kaj
      kompleza al pli potencaj:
      <ekz>
        <tld/>e ridetante, Wang Erdiao respondis <klr>...</klr>
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 84a, volumo 3a, p.
        59a</lok></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lake.0o.FIG">lakeo <sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>bezpáteřný</trd>,
    <trd>otrocký</trd>,
    <trd>patolízalský</trd>,
    <trd>podbízivý</trd>,
    <trd>ponížený</trd>,
    <trd>servilní</trd>
  </trdgrp>
  <trd lng="es">obsequioso</trd>
  <trdgrp lng="fa">
    <trd>&a_n;&a_w;&f_k;&a_r;&a_S;&a_f;&a_t;&a_A;&a_n;&a_h;</trd>,
    <trd>&a_b;&a_r;&a_d;&a_h;&zwnj;&a_w;&a_A;&a_r;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>obs&eacute;quieux</trd>,
    <trd>servile<klr> (obs&eacute;quieux)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szolgai</trd>,
    <trd>szolgalelk&udblac;</trd>,
    <trd>szervilis</trd>
  </trdgrp>
  <trd lng="it">servile</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_b;&c_o;&c_l;&c_je;&c_p;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_o;&c_b;&c_o;&c_s;&c_t;&c_r;&c_a;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_h;&c_o;&c_l;&c_o;&c_p;&c_s;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>otrocký</trd>,
    <trd>podlízavý</trd>
  </trdgrp>
</drv>
</art>

