<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>servil</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="servil.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="humil.0a.iu">Humila&ccirc;a</ref>, tro flatema kaj
      kompleza al pli potencaj:
      <ekz>
        <tld/>e ridetante, Wang Erdiao respondis <klr>...</klr>
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 84a, volumo 3a, p.
        59a</lok></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lake.0o.FIG">lakeo <sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="es">obsequioso</trd>
  <trdgrp lng="fr">
    <trd>obs&eacute;quieux</trd>,
    <trd>servile<klr> (obs&eacute;quieux)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szolgai</trd>,
    <trd>szolgalelk&udblac;</trd>,
    <trd>szervilis</trd>
  </trdgrp>
</drv>
</art>

