<art mrk="$Id: pandemi.xml,v 1.14 2024/02/16 19:24:10 revo Exp $">
<kap>
<rad>pandemi</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="pandemi.0o">
  <kap><tld/>o</kap>
  <snc mrk="pandemi.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Mondmalsano; <ref tip="sub" cel="epidem.0o.MED">epidemio</ref>,
      kiu vasti&gcirc;as al tuta terparto a&ubreve; al la mondo:
      <ekz>
        nuna <tld/>o de tuberkulozo
        <fnt>
        <aut>J. A. Vergara</aut>,
        <vrk>Sociaj aspektoj en la epidemologio de la aperantaj infektaj
        malsanoj</vrk>,
        <lok>83a UK, 1998-08-01 &gcirc;is 08</lok>
        </fnt>;
      </ekz>
      <ekz>
        lukti kontra&ubreve; la grandaj <tld/>oj
        <fnt>
        <aut>trad. D. Durand</aut>,
        <vrk>Monda agado kontra&ubreve; la mizero...</vrk>,
        <lok><url ref="http://www.globidar.org/solidareco/mseo85.htm">Monda
        solidareco, 2005-08</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_n;&c_d;&c_e;&c_m;&c_ib;&c_ja;</trd>
  <trd lng="cs">pandemie</trd>
  <trd lng="de">Pandemie</trd>
  <trd lng="en">pandemic</trd>
  <trdgrp lng="fa">
    <trd>&f_p;&a_A;&a_n;&a_d;&a_m;&f_y;</trd>,
    <trd>&a_h;&a_m;&a_h;&#x200C;&f_g;&f_y;&a_r;&f_y; &a_j;&a_h;&a_A;&a_n;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">pand&eacute;mie</trd>
  <trd lng="nl">pandemie</trd>
  <trd lng="pl">pandemia</trd>
  <trd lng="pt">pandemia</trd>
  <trd lng="sk">pand&eacute;mia</trd>
  <trd lng="uk">&#x43F;&c_a;&#x43D;&c_d;&c_je;&#x43C;&c_ib;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x5927;&#x6D41;&#x884C; <pr>d&#xE0;li&#xFA;x&#xED;ng</pr></trd>,
    <trd>&#x761F;&#x75AB; <pr>w&#x113;ny&#xEC;</pr></trd>,
    <trd>&#x5927;&#x6D41;&#x884C;&#x75C5; <pr>d&#xE0;li&#xFA;x&#xED;ngb&#xEC;ng</pr></trd>
  </trdgrp>
</drv>
</art>
