        <aut>trad. D. Durand</aut>,
        <vrk>Monda agado kontra&ubreve; la mizero...</vrk>,
        <lok><url
        ref="http://www.globidar.org/solidareco/mseo85.htm">Monda
        solidareco, 2005-08</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">pandemie</trd>
  <trd lng="en">pandemic</trd>
  <trdgrp lng="fa">
    <trd>&f_p;&a_A;&a_n;&a_d;&a_m;&f_y;</trd>,
    <trd>&a_h;&a_m;&a_h;&zwnj;&f_g;&f_y;&a_r;&f_y; &a_j;&a_h;&a_A;&a_n;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">pand&eacute;mie</trd>
  <trd lng="nl">pandemie</trd>
  <trd lng="pl">pandemia</trd>
  <trd lng="pt">pandemia</trd>
  <trd lng="sk">pand√©mia</trd>
