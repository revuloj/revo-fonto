<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: brunej.xml,v 1.16 2023/10/29 19:11:03 revo Exp $">
<kap>
  <rad>Brunej</rad>/o
  <fnt><bib>RLN</bib></fnt>
</kap>

<drv mrk="brunej.B0o">
  <kap>
    <tld lit="B"/>o
  </kap>
  <mlg kod="ISO-3166">BN</mlg>
  <snc mrk="brunej.B0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" cel="land.0o" lst="voko:&scirc;tatoj">Lando</ref> 
      en sudorienta <ref tip="malprt" cel="azi.A0o">Azio</ref>
      en la norda parto de insulo Borneo, kun &ccirc;efurbo Bandar-Seri-Begavano,
      limanta kun <ref tip="vid" cel="malajzi.M0o.GEOG">Malajzio</ref>
      kaj Sud&ccirc;ina Maro:
      <ekz>
        agadas apostolaj delegitoj (papaj reprezentantoj &ccirc;e lokaj katolikaj komunumoj,
        ne &ccirc;e la tiulanda registaro) <klr>[&#x2026;]</klr> en Azio (<tld/>o, Laoso, Malajzio
        kaj Birmo)
        <fnt>
          <bib>Monato</bib>
          <aut>Armando Zecchin</aut>
          <vrk><url ref="&Monato;/2009/009739.php">Vatikano diplomatie rilatas kun 177
          landoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        rilate al la nombro de la lo&gcirc;antaro la industria aldonvaloro de amba&ubreve; sektoroj
        okupis pintajn lokojn en Kataro, Kuvajto, Unui&gcirc;intaj Arabaj Emirlandoj,
        <tld/>o kaj Norvegio
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2014/011377.php">Modesta kresko de la mina industrio kaj de la
          sektoro de publikaj servoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la poluada indico en <tld/>o atingis 68 kaj iom influas la sanon de la popolo
        <fnt>
          <bib>Monato</bib>
          <aut>Syauqi Ahmad Zulfauzi Stya Lacksana</aut>
          <vrk><url ref="&Monato;/2015/011592.php">Kiam fini&gcirc;os la fumego?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
      <trd lng="zh">&#x6587;&#x83B1; <pr>w&#xE9;nl&#xE1;i</pr></trd>
</snc>
    <trd lng="be">&c_B;&c_r;&c_u;&c_n;&c_e;&c_j;</trd>
    <trd lng="bg">&c_B;&c_r;&c_u;&c_n;&c_je;&c_j;</trd>
    <trdgrp lng="ca">
      <trd>Brunei</trd>,
      <trd>Soldanat de Brunei</trd>
    </trdgrp>
    <trd lng="de">Brunei</trd>
    <trd lng="en">Brunei</trd>
    <trdgrp lng="fr">
      <trd>Brunei</trd>,
      <trd>Sultanat de Brunei</trd>
    </trdgrp>
    <trd lng="id">Brunei Darussalam</trd>
    <trd lng="ja">&#x30D6;&#x30EB;&#x30CD;&#x30A4;  <pr>&#x3076;&#x308B;&#x306D;&#x3044;</pr></trd>
  <trdgrp lng="pl">
      <trd>Brunei</trd>,
      <trd>Brunei Darussalam</trd>
    </trdgrp>
    <trd lng="pt">Brunei</trd>
  <trd lng="uk">&c_B;&c_r;&c_u;&#x43D;&c_je;&c_j;</trd>
</drv>

</art>

