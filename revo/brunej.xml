<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Brunej</rad>/o
</kap>

<drv mrk="brunej.B0o">
  <kap>
    <tld lit="B"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="brunej.B0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <mlg kod="ISO-3166">BN</mlg>
    <dif>
      <ref tip="lst" cel="land.0o" lst="voko:&scirc;tatoj">Lando</ref> 
      en sudorienta <ref tip="malprt" 
      cel="azi.A0o">Azio</ref> en la norda parto de insulo 
      <ref tip="malprt" cel="borne.0o">Borneo</ref>, kun
      &ccirc;efurbo Bandar-Seri-Begavano;
      &gcirc;i limas kun
      <ref tip="vid" cel="malajzi.M0o.GEOG">Malajzio</ref>
      kaj Sud&ccirc;ina Maro
    </dif>
    <trd lng="bg">&c_B;&c_r;&c_u;&c_n;&c_je;&c_j;</trd>
    <trdgrp lng="ca">
      <trd>Brunei</trd>,
      <trd>Soldanat de Brunei</trd>
    </trdgrp>
    <trd lng="de">Brunei</trd>
    <trd lng="en">Brunei</trd>
    <trd lng="id">Brunei Darussalam</trd>
    <trdgrp lng="pl">
      <trd>Brunei</trd>,
      <trd>Brunei Darussalam</trd>
    </trdgrp>
    <trd lng="pt">Brunei</trd>
  </snc>
</drv>

</art>

