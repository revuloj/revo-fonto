<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>brunej</rad>/o
</kap>

<drv mrk="brunej.0o">
  <kap>
    <tld lit="B"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="brunej.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la sudorienta <ref tip="malprt" 
      cel="azi.A0o">Azio</ref> en la norda parto de insulo Borneo, kun
      &ccirc;efurbo Bandar-Seri-Begavano.
      ISO-3166 kodo: BN.
      &Gcirc;i limas kun
      <ref tip="vid" cel="malajzi.0o.GEOG">Malajzio</ref>
      kaj Sud&ccirc;ina Maro
    </dif>
    <trdgrp lng="pl">
      <trd>Brunei</trd>,
      <trd>Brunei Darussalam</trd>
    </trdgrp>
  </snc>
</drv>

</art>

