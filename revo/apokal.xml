<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: apokal.xml,v 1.24 2023/10/13 16:46:48 revo Exp $">
<kap>
  <rad>apokalips</rad>/o
  <fnt>Z</fnt>
</kap>

<drv mrk="apokal.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BIB</uzo>
  <snc>
    <dif>
      Libro de la Nova Testamento, pri la fino de la mondo.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_a;&c_k;&c_a;&c_l;&c_ib;&c_p;&c_s;&c_ib;&c_s;</trd>
  <trdgrp lng="cs">
    <trd>apokalypsa</trd>,
    <trd>konec sv&#x11B;ta</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd><klr>Die </klr>Offenbarung des <ind>Johannes</ind></trd>,
     <trd>Offenbarung</trd>,
     <trd>Apokalypse</trd>
  </trdgrp>
  <trd lng="en">Book of <ind>Revelation</ind></trd>
  <trd lng="fr">apocalypse</trd>
  <trd lng="he">&#x5D0;&#x5B7;&#x5E4;&#x5BC;&#x5D5;&#x5B9;&#x5E7;&#x5B8;&#x5DC;&#x5B4;&#x5D9;&#x5E4;&#x5BC;&#x5E1;&#x5B8;&#x5D4;</trd>
  <trd lng="hu">apokalipszis</trd>
  <trd lng="pl">apokalipsa</trd>
  <trd lng="ru">&c_a;&c_p;&c_o;&c_k;&c_a;&c_l;&c_i;&c_p;&c_s;&c_i;&c_s;</trd>
  <trdgrp lng="sk">
    <trd>Apokalypsa</trd>,
    <trd>zjavenie sv. J&#xE1;na</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x430;&#x43F;&#x43E;&#x43A;&#x430;&#x43B;&#x456;&#x43F;&#x441;&#x438;&#x441;</trd>,
    <trd>&#x43E;&#x434;&#x43A;&#x440;&#x43E;&#x432;&#x435;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43E;&#x431;&#x2019;&#x44F;&#x432;&#x43B;&#x435;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="apokal.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">FIG</uzo>
  <snc>
    <dif>
      Terura, kiel la mondofino, kiel la Apokalipso priskribita en
      Biblio
    </dif>
    <trd lng="de">apokalyptisch</trd>
    <trdgrp lng="hu">
      <trd>apokaliptikus</trd>,
      <trd>v&eacute;g&iacute;t&eacute;letszer&udblac;</trd>
    </trdgrp>
    <trd lng="pl">apokaliptyczny</trd>
  </snc>
  <snc>
    <dif>
      Mistera, tute malklara:
      <ekz>
        <tld/>aj paroloj.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>wirr</trd>,
      <trd>dunkel</trd>,
      <trd>mysteri&ouml;s</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>apokaliptikus</trd>,
      <trd>l&aacute;tom&aacute;sszer&udblac;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_a;&c_k;&c_a;&c_l;&c_ib;&c_p;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>apokalyptick&#xFD;</trd>,
    <trd>katastrofick&#xFD;</trd>
  </trdgrp>
  <trd lng="de">apokalyptisch</trd>
  <trd lng="en">apocalyptic</trd>
  <trd lng="es">apocalipsis</trd>
  <trd lng="fr">apocalyptique</trd>
  <trd lng="he">&#x5D0;&#x5B7;&#x5E4;&#x5BC;&#x5D5;&#x5B9;&#x5E7;&#x5B8;&#x5DC;&#x5B4;&#x5D9;&#x5E4;&#x5BC;&#x5E1;&#x5B8;&#x5D4;</trd>
  <trdgrp lng="ja">
    <trd>&#x9ED9;&#x793A;&#x9332;&#x306E;  <pr>&#x3082;&#x304F;&#x3057;&#x308D;&#x304F;&#x306E;</pr></trd>,
    <trd>&#x96E3;&#x89E3;&#x306A;  <pr>&#x306A;&#x3093;&#x304B;&#x3044;&#x306A;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_p;&c_o;&c_k;&c_a;&c_l;&c_i;&c_p;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">apokalyptick&yacute;</trd>
</drv>
</art>
