<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>malavi</rad>/o
</kap>

<drv mrk="malavi.0o">
  <kap>
    <tld lit="M"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="malavi.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la sudorienta <ref tip="malprt"
      cel="afrik.A0o">Afriko</ref> 
      kun &ccirc;efurbo Lilongvo.
      Teritorio de Respubliko Malavio.
      ISO-3166 kodo: MW.
      Iam nomata Niaso.
      &Gcirc;i limas kun
      <ref tip="vid" cel="mozambik.0o.GEOG">Mozambiko</ref>,
      <ref tip="vid" cel="tanzani.0o.GEOG">Tanzanio</ref>,
      kaj <ref tip="vid" cel="zambi.0o.GEOG">Zambio</ref>.
    </dif>
    <trdgrp lng="pl">
      <trd>Malawi</trd>,
      <trd>Republika <ind>Malawi</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

