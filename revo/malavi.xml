<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Malavi</rad>/o
</kap>

<drv mrk="malavi.M0o">
  <kap>
    <tld lit="M"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="malavi.M0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o.POL">&Scirc;tato</ref> en la sudorienta 
      <ref tip="malprt" cel="afrik.A0o">Afriko</ref> 
      kun &ccirc;efurbo Lilongvo.
      Teritorio de Respubliko Malavio.
      ISO-3166 kodo: MW.
      Iam nomata Niaso.
      &Gcirc;i limas kun
      <ref tip="vid" cel="mozambik.M0o.GEOG">Mozambiko</ref>,
      <ref tip="vid" cel="tanzani.T0o.GEOG">Tanzanio</ref>,
      kaj <ref tip="vid" cel="zambi.Z0o.GEOG">Zambio</ref>.
    </dif>
    </snc>
    <trd lng="be">&c_M;&c_a;&c_l;&c_a;&c_v;&c_ib;</trd>
    <trd lng="bg">&c_M;&c_a;&c_l;&c_a;&c_v;&c_i;</trd>
    <trdgrp lng="ca">
      <trd>Malawi</trd>,
      <trd>Rep&uacute;blica de <ind>Malawi</ind></trd>
    </trdgrp>
    <trd lng="de">Malawi</trd>
    <trd lng="en">Malawi</trd>
    <trdgrp lng="fr">
    <trd>Malawi</trd>,
    <trd>R&eacute;publique du Malawi</trd>
    </trdgrp>
    <trd lng="id">Malawi</trd>
    <trd lng="nl">Malawi</trd>
    <trdgrp lng="pl">
      <trd>Malawi</trd>,
      <trd>Republika <ind>Malawi</ind></trd>
    </trdgrp>
    <trd lng="pt">Malawi</trd>
</drv>

</art>

