<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: skut.xml,v 1.21 2022/05/31 21:28:20 revo Exp $">
<kap>
  <rad>skut</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="skut.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="skut.0i.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="super" cel="vetur.0i.TRA">Veturi<sncref/></ref> per <tld/>ilo:
      <ekz>
        <nom>Marwine</nom> kaj mi <tld/>is dum unu semajno de la Nordmara bordo
        <klr>[&#8230;]</klr> al la Germana landlimo
        <fnt>
          <aut>R. Haveman</aut>
          <vrk><url ref="http://www.ipernity.com/blog/roel/82142">
          Skutado</url></vrk>
          <lok>2008-08-28</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_a;&c_c;&c_c;&c_a; &c_n;&c_a; 
    &c_s;&c_a;&c_m;&c_a;&c_k;&c_a;&c_c;&c_je;</trd>
  <trd lng="ca">patinet</trd>
  <trd lng="cs">jezdit na kolob&ecaron;&zcaron;ce</trd>
  <trdgrp lng="de">
      <trd>Roller <ind>fahren</ind></trd>,
      <trd>rollern</trd>
  </trdgrp>
  <trd lng="es">montar en patinete</trd>
  <trd lng="fr">trottiner<klr> (faire de la trottinette)</klr></trd>
  <trd lng="hu">rollerezik</trd>
  <trd lng="it">pattinare<klr> (con monopattino)</klr></trd>
  <trd lng="nl">steppen</trd>
  <trd lng="pl">je&zacute;dzi&cacute; na hulajnodze</trd>
  <trd lng="ru">&c_k;&c_a;&c_t;&c_a;&c_t;&c_mol;&c_s;&c_ja; &c_n;&c_a; 
<ind>&c_s;&c_a;&c_m;&c_o;&c_k;&c_a;&c_t;&c_je;</ind></trd>
  <trd lng="sk">kolobe&zcaron;kova&tcaron; sa</trd>
  <trd lng="uk">&c_k;&c_a;&c_t;&c_a;&c_t;&c_i;&c_s;&c_ja; &c_n;&c_a; &c_s;&c_a;&c_m;&c_o;&c_k;&c_a;&c_t;&c_ib;</trd>
</drv>

<drv mrk="skut.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="skut.0ilo.SPO">
    <dif>
      Durada <ref tip="lst" lst="voko:veturiloj" cel="vetur.0ilo.TRA">veturilo</ref>, 
      kun etaj radetoj, stirstango kaj piedplato, kiun
      oni irigas anta&ubreve;enpu&scirc;ante &gcirc;in per unu piedo:
      <ekz>
        placeto, kiu pleni&gcirc;os je knabetoj kun <klr>[s]</klr>iaj <tld/>iloj
        <fnt>
          <bib>MD11</bib>
          <vrk>Maliaj rifu&gcirc;intoj en la pariza &ccirc;irka&ubreve;a&jcirc;o</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la &leftquot;&ccirc;ududu&rightquot; <klr>[estas]</klr> duradaj veturiloj faritaj el ligno, nek biciklo,
        nek <tld/>ilo
        <fnt>
          <bib>MD14</bib>
          <vrk>La metamorfozoj de Goma</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bicikl.0o">biciklo</ref>,
      <ref cel="tricik.0o">triciklo</ref>,
       <ref cel="skoter.0o">skotero</ref>
    </refgrp>
    <bld lok="&WCU;/thumb/8/87/Razor_old_a.jpg/320px-Razor_old_a.jpg" prm="CC BY-SA 3.0">faldebla <tld/>ilo
        <fnt>
          <aut>&#19969;, propra verko</aut>
          <lok><url ref="&WCW;/File:Razor_old_a.jpg">&WCW;/File:Razor_old_a.jpg</url></lok>
        </fnt>
    </bld>
    <bld lok="&WCU;/thumb/8/89/Chukudu_in_North_Kivu.jpg/360px-Chukudu_in_North_Kivu.jpg" prm="CC BY-SA 2.0">transportado de bananoj sur <tld/>ilo &leftquot;&ccirc;u-ku-du&rightquot;
        <fnt>
          <aut>CIAT, DRC35_lo</aut>
          <lok><url ref="&WCW;/File:Chukudu_in_North_Kivu.jpg">&WCW;/File:Chukudu_in_North_Kivu.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_m;&c_a;&c_k;&c_a;&c_t;</trd>
  <trd lng="ca">patinar<klr> (amb patinet)</klr></trd>
  <trd lng="cs">kolob&ecaron;&zcaron;ka</trd>
  <trd lng="de">Roller</trd>
  <trd lng="es">patinete</trd>
  <trd lng="fr">trottinette</trd>
  <trd lng="hu">roller</trd>
  <trd lng="it">monopattino</trd>
  <trdgrp lng="nl">
    <trd>step</trd>,
    <trd>autoped</trd>
  </trdgrp>
  <trd lng="pl">hulajnoga</trd>
  <trd lng="ro">trotinet&abreve;</trd>
  <trd lng="ru">&c_s;&c_a;&c_m;&c_o;&c_k;&c_a;&c_t;</trd>
  <trd lng="sk">kolobe&zcaron;ka</trd>
  <trd lng="uk">&c_s;&c_a;&c_m;&c_o;&c_k;&c_a;&c_t;</trd>
</drv>

<drv mrk="skut.elektro0ilo">
  <kap>
    elektro<tld/>ilo,
    <var><kap>elektra <tld/>ilo</kap></var>
  </kap>
  <snc mrk="skut.elektro0ilo.SPO">
    <dif>
      <tld lit="S"/>ilo pelata per elektra motoro:
       <ekz>
        &ccirc;u vi hodia&ubreve; vidis la du elektrajn <tld/>ilojn, apud la bushaltejo?
        <fnt>
          <aut>E. Tantal</aut>
          <vrk><url ref="https://mega.nz/file/dNxCVZia#cwqJTBYWGqgSjQRp2NAHac459dgiK7tq45OxoopmtzI"
              >Misteroj - Elpensita a&ubreve; vera? (4)</url></vrk>
          <lok>Turka Stelo, 2022-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">trotinette &eacute;lectrique</trd>
  <trd lng="it">monopattino elettrico</trd>
</drv>
</art>
