<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>skut</rad>/i
  <fnt>PIV</fnt>
</kap>
<drv mrk="skut.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Veturi per <tld/>ilo.
    </dif>
  </snc>
  <trd lng="de">Roller fahren</trd>
  <trd lng="nl">steppen</trd>
  <trd lng="ru">&c_k;&c_a;&c_t;&c_a;&c_t;&c_mol;&c_s;&c_ja; &c_n;&c_a; <ind>&c_s;&c_a;&c_m;&c_o;&c_k;&c_a;&c_t;&c_je;</ind></trd>
</drv>

<drv mrk="skut.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Durada veturilo, kun etaj radetoj, stirstango kaj piedplato, kiun
      oni irigas anta&ubreve;enpu&scirc;ante &gcirc;in per unu piedo.
    </dif>
  </snc>
  <trd lng="de">Roller</trd>
  <trdgrp lng="nl">
    <trd>step</trd>,
    <trd>autoped</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_a;&c_m;&c_o;&c_k;&c_a;&c_t;</trd>
</drv>
</art>
