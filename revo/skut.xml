<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>skut</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="skut.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="skut.0i.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Veturi per <tld/>ilo:
      <ekz>
        Marwine kaj mi <tld/>is dum unu semajno de la Nordmara bordo
        <klr>...</klr> al la Germana landlimo
        <fnt>
          <aut>R. Haveman</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/roel/82142">
          Skutado</url></vrk>,
          <lok>2008-08-28</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_a;&c_c;&c_c;&c_a; &c_n;&c_a; 
    &c_s;&c_a;&c_m;&c_a;&c_k;&c_a;&c_c;&c_je;</trd>  
  <trd lng="ca">patinet</trd>  
  <trdgrp lng="de">
      <trd>Roller <ind>fahren</ind></trd>,
      <trd>rollern</trd>
  </trdgrp>
  <trd lng="fr">trottiner<klr> (faire de la trottinette)</klr></trd>
  <trd lng="hu">rollerezik</trd>
  <trd lng="it">pattinare<klr> (con monopattino)</klr></trd>
  <trd lng="nl">steppen</trd>
  <trd lng="ru">&c_k;&c_a;&c_t;&c_a;&c_t;&c_mol;&c_s;&c_ja; &c_n;&c_a; 
<ind>&c_s;&c_a;&c_m;&c_o;&c_k;&c_a;&c_t;&c_je;</ind></trd>
</drv>

<drv mrk="skut.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="skut.0ilo.SPO">
    <dif>
      Durada veturilo, 
      <ref tip="super" cel="cikl.0o.veturilo">ciklo</ref>
      kun etaj radetoj, stirstango kaj piedplato, kiun
      oni irigas anta&ubreve;enpu&scirc;ante &gcirc;in per unu piedo.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_m;&c_a;&c_k;&c_a;&c_t;</trd>
  <trd lng="ca">patinar<klr> (amb patinet)</klr></trd>
  <trd lng="de">Roller</trd>
  <trd lng="fr">trottinette</trd>
  <trdgrp lng="nl">
    <trd>step</trd>,
    <trd>autoped</trd>
  </trdgrp>
  <trd lng="hu">roller</trd>
  <trd lng="it">monopattino</trd>
  <trd lng="ru">&c_s;&c_a;&c_m;&c_o;&c_k;&c_a;&c_t;</trd>
</drv>
</art>
