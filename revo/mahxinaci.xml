<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ma&hcirc;inaci</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="mahxinaci.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Sekreta <ref tip="super" cel="intrig.0o.komploto">intrigo</ref> kun
      malica celo:
      <ekz>
        &ccirc;io &ccirc;i estis <tld/>o de don
        Fernando
        <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 27a, p.
        200a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_h;&c_ib;&c_n;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trd lng="ca">maquinaci&oacute;</trd>
  <trd lng="es">maquinaci&oacute;n</trd>
  <trd lng="fr">machination</trd>
  <trdgrp lng="hu">
    <trd>mesterked&eacute;s</trd>,
    <trd>fondorlat</trd>,
    <trd>cselsz&ouml;v&eacute;s</trd>,
    <trd>machin&aacute;ci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>maquina&ccedil;&atilde;o</trd>,
    <trd>cabala</trd>,
    <trd>conluio</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_a;&c_h;&c_i;&c_n;&c_a;&c_c;&c_i;&c_ja;</trd>
</drv>
</art>
