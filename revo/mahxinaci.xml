<vortaro>
<art mrk="$Id: mahxinaci.xml,v 1.15 2024/01/16 09:22:23 revo Exp $">
<kap>
  <rad>ma&hcirc;inaci</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="mahxinaci.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Sekreta <ref tip="super" cel="intrig.0o.komploto">intrigo</ref>
      kun malica celo:
      <ekz>
	&ccirc;io &ccirc;i estis <tld/>o de <nom>don Fernando</nom>
	<fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 27a, p.
	200a</lok></fnt>;
      </ekz>
      <ekz>
        ligo defenda kaj ataka kontra&ubreve; <klr>[&#8230;]</klr> komuna malamiko plena je dan&gcirc;eraj
        kaj misteraj <tld/>oj
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="insid.0o">insida&jcirc;o</ref>,
      <ref cel="komplo.0o">komploto</ref>,
      <ref cel="konspi.0o">konspiro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_h;&c_ib;&c_n;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_m;&c_a;&c_sh;&c_i;&c_n;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trd lng="ca">maquinaci&oacute;</trd>
  <trdgrp lng="de">
    <trd>Machenschaft</trd>,
    <trd>Machination</trd>,
    <trd>R&auml;nke</trd>,
    <trd>arglistige <ind>Umtriebe</ind></trd>,
    <trd>b&ouml;swillige <ind>Abmachung</ind></trd>
  </trdgrp>
  <trd lng="en">machination</trd>
  <trd lng="es">maquinaci&oacute;n</trd>
  <trd lng="fr">machination</trd>
  <trdgrp lng="hu">
    <trd>mesterked&eacute;s</trd>,
    <trd>fondorlat</trd>,
    <trd>cselsz&ouml;v&eacute;s</trd>,
    <trd>machin&aacute;ci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#38512;&#35584;  <pr>&#12356;&#12435;&#12412;&#12358;</pr></trd>,
    <trd>&#22904;&#31574;  <pr>&#12363;&#12435;&#12373;&#12367;</pr></trd>,
    <trd>&#24746;&#24039;&#12415;  <pr>&#12431;&#12427;&#12384;&#12367;&#12415;</pr></trd>
  </trdgrp>
  <trd lng="nl">machinatie</trd>
  <trd lng="pl">machinacja</trd>
  <trdgrp lng="pt">
    <trd>maquina&ccedil;&atilde;o</trd>,
    <trd>cabala</trd>,
    <trd>conluio</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_a;&c_h;&c_i;&c_n;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_a;&c_h;&c_ib;&c_n;&c_a;&c_c;&c_ib;&c_ja;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_s;&c_t;&c_u;&c_p;</trd>
  </trdgrp>
</drv>
</art>
