<kap>
  <rad>Mongoli</rad>/o
</kap>
<drv mrk="mongoli.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="mongol.M0ujo">Mongolujo</ref>:
      <ekz>
        la anta&ubreve;uloj de la turkecaj popoloj estis unueca popolo, kiu vivis sur la
        teritorio de <tld/>o kaj Altajo
        <fnt>

          <aut>Mihail Korotkov </aut>
          <vrk><url ref="&Monato;/2012/010634.php">La turka havas familion</url></vrk>
          <bib>Monato</bib>
          <lok>2012/08, p. 20</lok>
        </fnt>;
      </ekz>
      <ekz>
        plej altan rangon en japana tradicia luktado sumoo atingis junulo el <tld/>o
        <fnt>

          <aut>Igarasi Takeo</aut>
          <vrk><url ref="&Monato;/2004/008049.html">Sum(o)o plej alta</url></vrk>
          <bib>Monato</bib>
          <lok>2004/03, p. 13</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="en">Mongolia</trd>
  </snc>
  <trd lng="be">&c_M;&c_a;&c_n;&c_g;&c_o;&c_l;&c_ib;&c_ja;</trd>
  <trd lng="bo">&t_sa;&t_naro;&t_ga;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>
  <trd lng="cs">Mongolsko</trd>
  <trd lng="de">Mongolei</trd>
  <trd lng="fa">&a_m;&a_gh;&a_w;&a_l;&a_s;&a_t;&a_A;&a_n;</trd>
  <trd lng="fr">Mongolie</trd>
  <trd lng="hu">Mong&oacute;lia</trd>
  <trd lng="ja">&#x30E2;&#x30F3;&#x30B4;&#x30EB;  <pr>&#x3082;&#x3093;&#x3054;&#x308B;</pr></trd>
  <trd lng="nl">Mongoli&euml;</trd>
  <trd lng="pl">Mongolia</trd>
  <trd lng="pt">Mong&oacute;lia</trd>
  <trd lng="ru">&c_M;&c_o;&c_n;&c_g;&c_o;&c_l;&c_i;&c_ja;</trd>
