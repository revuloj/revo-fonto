    <trd>&#x8499;&#x53E4; <pr>M&ecaron;ngg&#x1D4;</pr></trd>,
    <trd>&#x8499;&#x53E4;&#x56FD; <pr>m&ecaron;ngg&#x1D4;gu&oacute;</pr></trd>,
    <trd>&#x8499;&#x53E4;&#x570B; <pr>m&ecaron;ngg&#x1D4;gu&oacute;</pr></trd>
