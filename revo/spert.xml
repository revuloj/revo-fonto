<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>spert</rad>/a
</kap>
<drv mrk="spert.0a">
   <kap><ofc>*</ofc><tld/>a</kap>
   <snc>
     <uzo tip="klr">(iu)</uzo>
     <dif>
       Posedanta sciojn kaj lertecon akiritajn per praktika observado kaj
       agado:
       <ekz>
         <tld/>a homo;
       </ekz>
       <ekz>
         ne demandu scienculon, demandu <tld/>ulon
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         sen<tld/>ulo
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="marcx.0o">mar&ccirc;o</ref>,
       <ref cel="frot">frotita</ref>,
       <ref cel="nuk.0o">nuko</ref>,
       <ref cel="paser.0o">pasero</ref>,
       <ref cel="pusx.0o">pu&scirc;o</ref>,
       <ref cel="tord.0i">tordi</ref>.
     </refgrp>
     <trd lng="hu">tapasztalt</trd>
   </snc>
   <snc>
     <uzo tip="klr">(io)</uzo>
     <dif>
       Montranta lertecon akiritan per praktikado:
       <ekz>
         <tld/>a gvidado.
       </ekz>
     </dif>
     <trd lng="hu">rutinos</trd>
   </snc>
   <trdgrp lng="be">
     <trd>&c_d;&c_a;&c_s;&c_mol;&c_v;&c_je;&c_d;&c_ch;&c_a;&c_n;&c_y;</trd>,
     <trd>&c_s;&c_p;&c_r;&c_a;&c_k;&c_t;&c_y;&c_k;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
   </trdgrp>
   <trd lng="cs">zku&scaron;en&yacute;</trd>
   <trd lng="de">erfahren</trd>
   <trd lng="en">experienced</trd>
   <trd lng="fr">exp&eacute;riment&eacute;</trd>
   <trdgrp lng="nl">
     <trd>ervaren</trd>,
     <trd>geoefend</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_o;&c_p;&c_y;&c_t;&c_n;&c_y;&c_j;</trd>,
     <trd>&c_s;&c_v;&c_je;&c_d;&c_u;&c_shch;&c_i;&c_j;</trd>
   </trdgrp>
</drv>

<drv mrk="spert.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc>
     <dif>
       Fari&gcirc;i <tld/>a pri.
     </dif>
     <trd lng="de">erfahren</trd>
     <trd lng="nl">oefenen</trd>
     <trd lng="ru">&c_p;&c_r;&c_i;&c_o;&c_b;&c_r;&c_je;&c_s;&c_t;&c_i; 
<ind>&c_o;&c_p;&c_y;&c_t;</ind></trd>
   </snc>
   <snc>
     <dif>
       Travivi, suferi a&ubreve; &gcirc;ui, senti, konstati:
       <ekz>
         <tld/>i krizon, multajn malfeli&ccirc;ojn, malhelpojn, atakojn,
         novan seniluzii&gcirc;on<fnt>Z</fnt>;
       </ekz>
       <ekz>
         mi mem <tld/>is tian embarason;
       </ekz>
       <ekz>
         mi persone <tld/>is la facilecon de esp.;
       </ekz>
       <ekz>
         la alia &scirc;ipo <tld/>is la saman sorton
       </ekz>
       <trdgrp lng="de">
         <trd>erfahren</trd>,
         <trd>erleben</trd>,
         <trd>durchmachen</trd>
       </trdgrp>
       <trd lng="nl">ervaren</trd>
       <trdgrp lng="ru">
         <trd>&c_p;&c_je;&c_r;&c_je;&c_zh;&c_i;&c_t;&c_mol;</trd>,
         <trd>&c_i;&c_s;&c_p;&c_y;&c_t;&c_a;&c_t;&c_mol; &c_n;&c_a; 
&c_s;&c_je;&c_b;&c_je;</trd>,
         <trd>&c_u;&c_z;&c_n;&c_a;&c_t;&c_mol; &c_n;&c_a; 
&c_s;&c_o;&c_b;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;&c_m; 
<ind>&c_o;&c_p;&c_y;&c_t;&c_je;</ind></trd>
       </trdgrp>
     </dif>
     <refgrp tip="sin">
       <ref cel="pas.tra0i">trapasi</ref>,
       <ref cel="traf.0i">trafi</ref>.
     </refgrp>
     <refgrp tip="vid">
       <ref cel="posx.0o">po&scirc;o</ref>.
     </refgrp>
   </snc>
   <trdgrp lng="be">
     <trd>&c_n;&c_a;&c_b;&c_y;&c_c;&c_mol; 
<ind>&c_d;&c_o;&c_s;&c_mol;&c_v;&c_je;&c_d;</ind></trd>,
     <trd>&c_p;&c_a;&c_z;&c_n;&c_a;&c_c;&c_mol; &c_z; 
&c_u;&c_l;&c_a;&c_s;&c_n;&c_a;&c_g;&c_a; 
<ind>&c_d;&c_o;&c_s;&c_mol;&c_v;&c_je;&c_d;&c_u;</ind></trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>tapasztal</trd>,
     <trd>&aacute;t&eacute;l</trd>
   </trdgrp>
</drv>

<drv mrk="spert.0o">
   <kap><tld/>o</kap>
   <snc>
     <dif>
       Scioj kaj lerteco akiritaj per praktika observado kaj agado:
       <ekz>
         <tld/>o estas averto<fnt>Z</fnt>;
       </ekz>
       <ekz>
         <ind><tld/>o sa&gcirc;on akrigas</ind>
         <fnt><bib>PrV</bib></fnt>.
         <trd lng="hu">tapasztalatt&oacute;l n&odblac; a 
b&ouml;lcsess&eacute;g</trd>
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="brog.0i">brogi</ref>,
       <ref cel="kornik.0o">korniko</ref>,
       <ref cel="vund.0o">vundo</ref>
     </refgrp>
   </snc>
   <snc>
     <dif>
       Traviva&jcirc;o:
       <ekz>
         li memoros la <tld/>on.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_d;&c_o;&c_s;&c_mol;&c_v;&c_je;&c_d;</trd>
   <trd lng="de">Erfahrung</trd>
   <trd lng="hu">tapasztalat</trd>
   <trd lng="nl">ervaring</trd>
   <trd lng="fr">exp&eacute;rience</trd>
   <trd lng="ru">&c_o;&c_p;&c_y;&c_t;</trd>
</drv>

<drv mrk="spert.0ajxo">
   <kap><tld/>a&jcirc;o</kap>
   <snc>
     <dif>
       Lerta procedo eltrovita per praktikado.
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_d;&c_o;&c_s;&c_mol;&c_v;&c_je;&c_d;</trd>,
     <trd>&c_s;&c_p;&c_r;&c_a;&c_k;&c_t;&c_y;&c_k;&c_a;&c_v;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
   </trdgrp>
   <trd lng="ru">&c_o;&c_p;&c_y;&c_t;</trd>
   <trd lng="fr"><ind>tour</ind> de main</trd>
</drv>
</art>
