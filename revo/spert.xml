<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: spert.xml,v 1.27 2021/05/28 17:06:21 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>spert</rad>/a <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="spert.0a">
   <kap><ofc>*</ofc><tld/>a</kap>
   <snc mrk="spert.0a.KOMUNE">
     <uzo tip="klr">(iu)</uzo>
     <dif>
       Posedanta sciojn kaj lertecon akiritajn per praktika observado
       kaj
       agado:
       <ekz>
         <tld/>a homo;
       </ekz>
       <ekz>
         ne demandu scienculon, demandu <tld/>ulon
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         sen<tld/>ulo
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="kompet.0a.kapabla">kompetenta</ref>,
       <ref cel="eksper2.0isto">ekspertizisto</ref>
     </refgrp>
     <trd lng="hu">tapasztalt</trd>
   </snc>
   <snc mrk="spert.0a.spertula">
     <uzo tip="klr">(io)</uzo>
     <dif>
       <tld lit="S"/>ula, montranta lertecon akiritan per praktikado:
       <ekz>
         <tld/>a gvidado.
       </ekz>
     </dif>
     <trd lng="hu">rutinos</trd>
   </snc>
  <trdgrp lng="be">
     <trd>&c_d;&c_a;&c_s;&c_mol;&c_v;&c_je;&c_d;&c_ch;&c_a;&c_n;&c_y;</trd>,
     <trd>&c_s;&c_p;&c_r;&c_a;&c_k;&c_t;&c_y;&c_k;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>zb&#x11B;hl&#xFD;</trd>,
    <trd>zku&scaron;en&yacute;</trd>,
    <trd>znal&#xFD; <klr>(odborn&#x11B;)</klr></trd>
  </trdgrp>
  <trd lng="de">erfahren</trd>
  <trd lng="en">experienced</trd>
  <trd lng="es">experimentado, -a</trd>
  <trd lng="fr">exp&eacute;riment&eacute;</trd>
  <trdgrp lng="nl">
     <trd>ervaren</trd>,
     <trd>geoefend</trd>
   </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_o;&c_p;&c_y;&c_t;&c_n;&c_y;&c_j;</trd>,
     <trd>&c_s;&c_v;&c_je;&c_d;&c_u;&c_shch;&c_i;&c_j;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>ostrie&#x13E;an&#xFD;</trd>,
    <trd>sk&#xFA;sen&#xFD;</trd>
  </trdgrp>
</drv>

<drv mrk="spert.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc>
     <dif>
       Fari&gcirc;i <tld/>a pri.
     </dif>
     <trd lng="de">erfahren</trd>
     <trd lng="es">experimentar en</trd>
     <trd lng="nl">oefenen</trd>
     <trd lng="ru">&c_p;&c_r;&c_i;&c_o;&c_b;&c_r;&c_je;&c_s;&c_t;&c_i; <ind>&c_o;&c_p;&c_y;&c_t;</ind></trd>
   </snc>
   <snc>
     <dif>
       <ref tip="vid" cel="viv.tra0i">Travivi</ref>, suferi a&ubreve; &gcirc;ui, 
       senti, konstati:
       <ekz>
         <tld/>i krizon, multajn malfeli&ccirc;ojn, malhelpojn, atakojn,
         novan seniluzii&gcirc;on<fnt>Z</fnt>;
       </ekz>
       <ekz>
         mi mem <tld/>is tian embarason;
       </ekz>
       <ekz>
         mi persone <tld/>is la facilecon de Esperanto;
       </ekz>
       <ekz>
         la alia &scirc;ipo <tld/>is la saman sorton
       </ekz>
       <refgrp tip="vid">
         <ref cel="pas.tra0i">trapasi</ref>,
         <ref cel="traf.0i">trafi</ref>
       </refgrp>
     </dif>
     <trdgrp lng="de">
       <trd>erfahren</trd>,
       <trd>erleben</trd>,
       <trd>durchmachen</trd>
     </trdgrp>
     <trd lng="nl">ervaren</trd>
     <trd lng="es">experimentar</trd>
     <trdgrp lng="ru">
       <trd>&c_p;&c_je;&c_r;&c_je;&c_zh;&c_i;&c_t;&c_mol;</trd>,
       <trd>&c_i;&c_s;&c_p;&c_y;&c_t;&c_a;&c_t;&c_mol; &c_n;&c_a; &c_s;&c_je;&c_b;&c_je;</trd>,
       <trd>&c_u;&c_z;&c_n;&c_a;&c_t;&c_mol; &c_n;&c_a; &c_s;&c_o;&c_b;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;&c_m; <ind>&c_o;&c_p;&c_y;&c_t;&c_je;</ind></trd>
     </trdgrp>
   </snc>
   <refgrp tip="vid">
     <ref cel="progre.0i">progresi</ref>
   </refgrp>
   <trdgrp lng="be">
     <trd>&c_n;&c_a;&c_b;&c_y;&c_c;&c_mol; <ind>&c_d;&c_o;&c_s;&c_mol;&c_v;&c_je;&c_d;</ind></trd>,
     <trd>&c_p;&c_a;&c_z;&c_n;&c_a;&c_c;&c_mol; &c_z; &c_u;&c_l;&c_a;&c_s;&c_n;&c_a;&c_g;&c_a; <ind>&c_d;&c_o;&c_s;&c_mol;&c_v;&c_je;&c_d;&c_u;</ind></trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>&eacute;prouver</trd>,
     <trd>&ecirc;tre <ind>exp&eacute;riment&eacute;</ind> en</trd>,
     <trd>faire l'<ind>exp&eacute;rience</ind> de</trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>tapasztal</trd>,
     <trd>&aacute;t&eacute;l</trd>
   </trdgrp>
  <trd lng="ne">&#x905;&#x928;&#x941;&#x92D;&#x935;</trd>
  <trdgrp lng="sk">
    <trd>pre&#x17E;i&#x165;</trd>,
    <trd>sk&#xFA;si&#x165;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>poznat</trd>,
    <trd>proc&#xED;tit</trd>,
    <trd>pro&#x17E;&#xED;t</trd>,
    <trd>zakusit</trd>
  </trdgrp>
</drv>

<drv mrk="spert.0o">
   <kap><tld/>o</kap>
   <snc>
     <dif>
       Scioj kaj lerteco akiritaj per <ref tip="vid" cel="prakti.0o">praktika</ref> observado kaj agado:
       <ekz>
         <tld/>o estas averto<fnt>Z</fnt>;
       </ekz>
       <ekz>
         <ind><tld/>o sa&gcirc;on akrigas</ind>
         <fnt><bib>PrV</bib></fnt>.
         <trd lng="hu">tapasztalatt&oacute;l n&odblac; a b&ouml;lcsess&eacute;g</trd>
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="brog.0i">brogi</ref>,
       <ref cel="vund.0o">vundo</ref>
     </refgrp>
   </snc>
   <snc>
     <dif>
       Traviva&jcirc;o:
       <ekz>
         li memoros la <tld/>on.
       </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_d;&c_o;&c_s;&c_mol;&c_v;&c_je;&c_d;</trd>
  <trd lng="bg">&c_o;&c_p;&c_i;&c_t;</trd>
  <trd lng="cs">zku&#x161;enost</trd>
  <trd lng="de">Erfahrung</trd>
  <trd lng="es">experiencia</trd>
  <trdgrp lng="fr">
     <trd>exp&eacute;rience<klr> (v&eacute;cue)</klr></trd>,
     <trd>v&eacute;cu<klr> (subst.)</klr></trd>
   </trdgrp><trd lng="fr">exp&eacute;rience</trd>
  <trd lng="hu">tapasztalat</trd>
  <trd lng="nl">ervaring</trd>
  <trd lng="ru">&c_o;&c_p;&c_y;&c_t;</trd>
  <trd lng="sk">sk&#xFA;senos&#x165;</trd>
</drv>

<drv mrk="spert.0ajxo">
   <kap><tld/>a&jcirc;o</kap>
   <snc>
     <dif>
       Lerta procedo eltrovita per praktikado.
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_d;&c_o;&c_s;&c_mol;&c_v;&c_je;&c_d;</trd>,
     <trd>&c_s;&c_p;&c_r;&c_a;&c_k;&c_t;&c_y;&c_k;&c_a;&c_v;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
   </trdgrp>
   <trd lng="es">pr&aacute;ctica</trd>
   <trd lng="fr"><ind>tour</ind> de main</trd>
   <trd lng="ru">&c_o;&c_p;&c_y;&c_t;</trd>
</drv>

<drv mrk="spert.fak0a">
  <kap>fak<tld/>a</kap>
  <snc mrk="spert.fak0a.KOMUNE">
    <dif>
      <tld lit="S"/>a pri aparta fako:
      <ekz>
        &scirc;i priklis <klr>[rizplantojn]</klr> tiel rapide kaj 
        lerte kiel iu ajn fak<tld/>a terkulturisto
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">chevronn&eacute;<klr> (exp&eacute;riment&eacute;)</klr></trd>
</drv>

<drv mrk="spert.mal0eco">
  <kap>mal<tld/>eco, <var><kap>sen<tld/>eco</kap></var></kap>
  <snc mrk="spert.mal0eco.KOMUNE">
    <dif>
      Stato de iu ne havinta <tld/>ojn, ne konanta per propra 
      travivado:
      <ekz>
        &gcirc;uste pro nia sen<tld/>eco, iuj malsimplaj aferoj &scirc;ajnis al ni 
        tute simplaj
        <fnt><aut>R. Radiguet, trad. M. Duc Goninaz</aut>, <vrk>Kun 
        Diablo en la korpo</vrk>, <lok>2006</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">nezku&#x161;enost</trd>
  <trd lng="es">inexperiencia</trd>
  <trd lng="fr">inexp&eacute;rience</trd>
  <trd lng="sk">nesk&#xFA;senos&#x165;</trd>
</drv>
</art>
