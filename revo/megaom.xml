<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: megaom.xml,v 1.13 2023/09/02 06:04:10 revo Exp $">
<kap>
  <rad>megaom</rad>/o,
  <var><kap>megom/o <fnt><bib>PV</bib></fnt></kap></var>
</kap>
<drv mrk="megaom.0o">
  <kap><tld/>o,
   <var><kap>megomo</kap></var>
  </kap>
  <snc mrk="megaom.0o.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de elektra
      rezistanco, egala al unu miliono de 
      <ref tip="prt" cel="om.o0o">omoj</ref>:
      <ekz>
        la nomoj de dekoblaj kaj dekonaj mezurunuoj de omo deriveblas per 
        la kutimaj prefiksoj de SI, la plej ofte uzataj el ili, en la fako de elektroniko, estas miliomo, kiloomo kaj <tld/>o 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=47298">Omo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_g;&c_a;&c_o;&c_m;</trd>
  <trd lng="ca">megaohm</trd>
  <trd lng="cs">megaohm</trd>
  <trd lng="de">Megaohm</trd>
  <trd lng="en">megohm</trd>
  <trd lng="es">megaohmio</trd>
  <trd lng="fa">&a_m;&f_g;&a_A;&a_A;&a_damma;&a_h;&a_m;</trd>
  <trd lng="fr">m&eacute;gohm</trd>
  <trd lng="hu">megohm</trd>
  <trd lng="ja">&#x30E1;&#x30AC;&#x30AA;&#x30FC;&#x30E0;</trd>
  <trd lng="nl">mega-ohm</trd>
  <trd lng="pl">megaom</trd>
  <trdgrp lng="pt">
    <trd>megohm</trd>,
    <trd>megaohm</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_je;&c_g;&c_a;&c_o;&c_m;</trd>
  <trd lng="sk">megaohm</trd>
  <trd lng="uk">&#x43C;&#x435;&#x491;&#x430;&#x43E;&#x43C;</trd>
</drv>
</art>

