<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tromp</rad>/i
</kap>
<drv mrk="tromp.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="tromp.0i.KOMUNE">
    <dif>
      Erarigi iun, kredigi ion malefektivan, ofte ruze kaj malice:
      <ekz>
        <tld/>i la a&ccirc;etanton<fnt>B</fnt>;
      </ekz>
      <ekz>
        ni ne lasu nin <tld/>i per tiaj belaj promesoj;
      </ekz>
      <ekz>
        <tld/>e <klr>(por <tld/>i)</klr> ka&scirc;i sian malamon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        vi ne havas edzinon, kiu vin <tld/>as
        <klr>(estas al vi <ref cel="fidel.mal0a">malfidela</ref>)</klr>;
        (<refgrp tip="vid">
          <ref cel="kok.0o.AGR">koko</ref>,
          <ref cel="kokr.0i">kokri</ref>
        </refgrp>)
        <fnt><bib>FK</bib>, <lok>4, rakontoj, la porcio da
        glacia&jcirc;o</lok></fnt>;
      </ekz>
      <ekz>
      </ekz>
      <ekz>
        <ind>&scirc;ajno <tld/>as</ind>
        <trd lng="fr">les apparences sont trompeuses</trd>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i la esperojn
        <klr>(ka&ubreve;zi elrevi&gcirc;on)</klr>
        <fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Jakob <tld/>is <klr>(sukcese evitis)</klr> la atenton de Laban
        <klr>...</klr>
        ne dirante al li, ke li forkuras
        <fnt><bib>MT</bib>, <lok>&Gen; 31:20</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fripon.0o">fripono</ref>,
      <ref cel="mistif.0i">mistifiki</ref>,
      <ref cel="perfid.0i">perfidi</ref>,
      <ref cel="polv.0o">polvo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_sh;&c_u;&c_k;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_m;&c_a;&c_n;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>entt&auml;uschen</trd>,
    <trd>t&auml;uschen</trd>,
    <trd>betr&uuml;gen</trd>,
    <trd>hintergehen</trd>
  </trdgrp>
  <trd lng="en">deceive</trd>
  <trdgrp lng="es">
    <trd>enga&ntilde;ar</trd>,
    <trd>hacer <ind>trampa</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>abuser</trd>,
    <trd>duper</trd>,
    <trd>tromper</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>becsap</trd>,
    <trd>r&aacute;szed</trd>,
    <trd>megt&eacute;veszt</trd>,
    <trd>csal</trd>,
    <trd lng="hu">megcsal</trd>,
    <trd lng="hu">megker&uuml;l</trd>
  </trdgrp>
  <trdgrp lng="lat">
    <trd>decipio, ere</trd>,
    <trd>fallo, ere</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bedriegen</trd>,
    <trd>ontwijken</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>enganar</trd>,
    <trd>iludir</trd>,
    <trd>burlar</trd>,
    <trd>embair</trd>,
    <trd>fraudar</trd>,    
    <trd>lograr</trd>,    
    <trd>ludibriar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_m;&c_a;&c_n;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_m;&c_a;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="tromp.0a">
  <kap><tld/>a</kap>
  <snc mrk="tromp.0a.KOMUNE">
    <dif>
      <tld lit="T"/>anta:
      <ekz>
        &ccirc;armeco estas <tld/>a<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a &scirc;ajno, kvieto;
      </ekz>
      <ekz>
        &gcirc;i estas <tld/>a pano<fnt>Z</fnt>
        <klr>(&scirc;ajna, ne efektiva bono)</klr>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_m;&c_a;&c_n;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_z;&c_m;&c_a;&c_n;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_a;&c_sh;&c_u;&c_k;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>t&auml;uschend</trd>,
    <trd>betr&uuml;gerisch</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>trompeur</trd>,
    <trd>fallacieux</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csal&oacute;</trd>,
    <trd>megt&eacute;veszt&odblac;</trd>
  </trdgrp>
  <trdgrp lng="lat">
    <trd>dolosus</trd>,
    <trd>fallax</trd>
  </trdgrp>
  <trd lng="nl">bedrieglijk</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_m;&c_a;&c_n;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_b;&c_m;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="tromp.0igxi">
  <kap><ofc>*</ofc><tld/>i&gcirc;i</kap>
  <snc mrk="tromp.0igxi.KOMUNE">
    <dif>
      Esti <tld/>ita.
    </dif>
    <trd lng="de">get&auml;uscht werden</trd>
  </snc>
  <snc mrk="tromp.0igxi.erari">
    <uzo tip="stl">EVI</uzo>
    <ref tip="dif" cel="erar.0i.opinii">erari <sncref/></ref>
    <ekz>
      balda&ubreve; ni vidis, ke ni <tld/>i&gcirc;is
      <fnt><bib>F</bib>, <lok>&FE; 40</lok></fnt>.
    </ekz>
    <trdgrp lng="de">
      <trd><klr>sich </klr>t&auml;uschen</trd>,
      <trd><klr>sich </klr>irren</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_sh;&c_u;&c_k;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_m;&c_a;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="hu">csal&oacute;dik</trd>
  <trd lng="ru">&c_o;&c_b;&c_m;&c_a;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="tromp.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc>
    <dif>
      Elrevi&gcirc;o, seniluzii&gcirc;o.
    </dif>
  </snc>
  <trd lng="de">Entt&auml;uschung</trd>
  <trd lng="hu">csal&oacute;d&aacute;s</trd>
</drv>
</art>
