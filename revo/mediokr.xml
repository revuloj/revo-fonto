<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mediokr.xml,v 1.7 2024/01/25 20:10:35 revo Exp $">
<kap>
  <rad>mediokr</rad>/a <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="mediokr.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="mediokr.0a.KOMUNE">
    <dif>
      Meza&ccirc;a, malbrila:
      <ekz>
        oni nur sciis, ke li estis iom <tld/>a oficisto, ke li ridas, 
        ke li fumas, kaj ke foje li faris maldeca&jcirc;ojn
        <fnt>
          <aut>L. Couperus, trad. G. Berveling</aut>,
          <vrk>Pri Maljunuloj, la A&jcirc;oj kiuj Pasas</vrk>,
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        la a&ubreve;toro elfor&gcirc;as mondon plenan de malri&ccirc;o kaj <tld/>o
        <fnt>
          <vrk><url ref="https://www.nodo50.org/esperanto/Libroservo/Suzana.htm">Suzana kaj la &ccirc;asistoj de mu&scirc;oj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_s;&c_r;&c_e;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_ja;&c_r;&c_e;&c_d;&c_n;&c_ib;</trd>,
    <trd>&c_s;&c_ja;&c_r;&c_e;&c_d;&c_n;&c_ib;&c_h; &c_z;&c_d;&c_o;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_w;</trd>
  </trdgrp>
  <trd lng="de">mittelm&auml;&szlig;iger <klr>(Mensch)</klr></trd>
  <trd lng="en">mediocre</trd>
  <trdgrp lng="fr">
    <trd>m&eacute;diocre</trd>,
    <trd>pi&egrave;tre</trd>
  </trdgrp>
  <trd lng="pl">przeci&eogonek;tny</trd>
  <trd lng="ro">mediu</trd>
  <trdgrp lng="zh">
    <trd>&#x5E73;&#x5E73; <pr>p&#xED;ngp&#xED;ng</pr></trd>,
    <trd>&#x5E73;&#x5EB8; <pr>p&#xED;ngy&#x14D;ng</pr></trd>,
    <trd>&#x788C;&#x788C;&#x65E0;&#x4E3A; <pr>l&#xF9;l&#xF9;w&#xFA;w&#xE9;i</pr></trd>,
    <trd>&#x788C;&#x788C;&#x7121;&#x70BA; <pr>l&#xF9;l&#xF9;w&#xFA;w&#xE9;i</pr></trd>,
    <trd>&#x8F03;&#x5DEE; <pr>ji&#xE0;och&#xE0;</pr></trd>,
    <trd>&#x8F83;&#x5DEE; <pr>ji&#xE0;och&#xE0;</pr></trd>,
    <trd>&#x4E2D;&#x8DEF; <pr>zh&#x14D;ngl&#xF9;</pr></trd>,
    <trd>&#x5EB8; <pr>y&#x14D;ng</pr></trd>,
    <trd>&#x666E;&#x666E;&#x901A;&#x901A; <pr>p&#x1D4;p&#x1D4;t&#x14D;ngt&#x14D;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mediokr.0ulo">
  <kap>
    <tld/>ulo
  </kap>
  <snc>
    <dif>
      Malbrila, meza&ccirc;a homo; <ref tip="sin" cel="averagx.0ulo">avera&gcirc;ulo</ref>:
      <ekz>
        li skribas nek por naivaj klubanoj ie en perdita vila&gcirc;o, nek por &scirc;ajnprudaj
        <tld/>uloj
        <fnt>
          <bib>Monato</bib>
          <aut>Raita Pyh&auml;l&auml;</aut>
          <vrk><url ref="&Monato;/2005/008685.php">Kesto de viveroj</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_r;&c_e;&c_d;&c_n;&c_y; &c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;</trd>
  <trdgrp lng="de">
    <trd>Mittelm&auml;&szlig;iger</trd>,
    <trd>Unkreativer</trd>,
    <trd>Dumpfbacke</trd>
  </trdgrp>
</drv>


</art>

