<vortaro>
<art mrk="$Id: go.xml,v 1.19 2018/12/19 21:10:14 revo Exp $">
<kap>
  <rad>go</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="go.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      Strategia surtabla <ref tip="super" cel="lud.0o">ludo</ref>
      el Orienta Azio (ver&scirc;ajne &Ccirc;inujo), por du
      ludantoj, kiuj alterne surtabuligas respektive 181 nigrajn kaj 180
      blankajn &scirc;tonetojn, celante &ccirc;irka&ubreve;marki la&ubreve;eble vastan surfacon:
      <ekz>
        la Esperantista <tld lit="G"/>-Ligo Internacia (EGLI) fondi&gcirc;is en 1979 kaj estas
        faka asocio de UEA, kiu celas disvastigi Esperanton per la ludo <tld/>o, kaj
        <tld/>on per Esperanto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=115872"
              >Esperantista Go-Ligo Internacia</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="vejcxi.0o">vej&ccirc;io</ref>
    </refgrp>
    <bld
      lok="&WCU;/thumb/e/e1/Go-result.JPG/360px-Go-result.JPG"
      prm="CC BY-SA 2.0"
      >rezulto de <tld/>ludo
        <fnt>
          <lok><url ref="&WCW;/File:Go-result.JPG"
              >https://commons.wikimedia.org/wiki/File:Go-result.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_g;&c_o;</trd>
  <trd lng="bg">&c_g;&c_o;</trd>
  <trd lng="ca">go</trd>
  <trd lng="cs">go</trd>
  <trd lng="de">Go</trd>
  <trd lng="en">go</trd>
  <trd lng="fr">go</trd>
  <trd lng="hu">g&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#30849; [<ind>&#12372;</ind>]</trd>,
    <trd>&#22258;&#30849; [<ind>&#12356;&#12372;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="ko">
    <trd>&#48148;&#46161;</trd>,
    <trd>baduk</trd>
  </trdgrp>
  <trd lng="nl">go</trd>
  <trd lng="pl">go<klr tip="ind"> (gra)</klr></trd>
  <trd lng="pt">go<klr tip="amb"> (jogo chin&ecirc;s)</klr></trd>
  <trd lng="ru">&c_g;&c_o;</trd>
  <trd lng="sk">go</trd>
  <trdgrp lng="zh">
    <trd>&#22285;&#26827; <klr>(trad.)</klr></trd>,
    <trd>&#22260;&#26827; [<ind>w&eacute;iq&iacute;</ind>] <klr>(simp.)</klr></trd>,
  </trdgrp>
</drv>
</art>
