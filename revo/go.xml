<art mrk="$Id$">
<kap>
  <rad>go</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="go.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      Kombina surtabla ludo el Orienta Azio,
      por du ludantoj, kiuj alterne surtabuligas respektive
      181 nigrajn kaj 180 blankajn &scirc;tonetojn, celante
      &ccirc;irka&ubreve;marki plejeble vastan surfacon.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>go</trd>
  </trdgrp>
</drv>
</art>

