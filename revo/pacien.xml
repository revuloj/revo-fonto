<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc><rad>pacienc</rad>/o</kap>
<drv mrk="pacien.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Virto de tiu, kiu rezignacie kaj trankvile eltenas malbonon:
      <ekz>
        havi, montri an&gcirc;elan <tld/>on;
      </ekz>
      <ekz>
        li el&ccirc;erpis mian <tld/>on<fnt>B</fnt>;
      </ekz>
      <ekz>
        la unuaj esperantistoj <tld/>e elmetadis sin al konstanta
        mokado<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Eco de tiu, kiu kviete atendas iun a&ubreve; ion malfrui&gcirc;antan:
      <ekz>
        li atendis min <tld/>e.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Persisto:
      <ekz>per <tld/>o kaj fervoro sukcesas &ccirc;iu laboro
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>per <tld/>o venas scienco
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        nia laborado ankora&ubreve; longe postulados de ni feran
        <tld/>on<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="obstin.0a">obstina</ref>,
      <ref cel="infer.0a">infera</ref>,
      <ref cel="sxtal.0a">&scirc;tala</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Geduld</trd>
  <trd lng="nl">geduld</trd>
  <trd lng="ru">&c_t;&c_je;&c_r;&c_p;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sv">t&aring;lamod</trd>
</drv>

<drv mrk="pacien.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Havanta <tld/>on.
    </dif>
  </snc>
  <trd lng="de">geduldig</trd>
  <trd lng="nl">geduldig</trd>
  <trd lng="ru">&c_t;&c_je;&c_r;&c_p;&c_je;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
</drv>

<drv mrk="pacien.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>a.
    </dif>
  </snc>
  <trd lng="de">sich <ind>gedulden</ind></trd>
  <trd lng="nl">geduld hebben</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_je;&c_r;&c_p;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_n;&c_o;&c_s;&c_i;&c_t;&c_mol;&c_s;&c_ja; &c_s; <ind>&c_t;&c_je;&c_r;&c_p;&c_je;&c_n;&c_i;&c_je;&c_m;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="pacien.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Ne havanta <tld/>on:
      <ekz>
        li mal<tld/>e iris kaj reiris
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pland.0o">plando</ref>,
      <ref cel="pingl.0o">pinglo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">ungeduldig</trd>
  <trd lng="nl">ongeduldig</trd>
  <trd lng="ru">&c_n;&c_je;&c_t;&c_je;&c_r;&c_p;&c_je;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
</drv>
</art>
