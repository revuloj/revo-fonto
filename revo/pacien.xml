<drv mrk="pacien.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="pacien.0i.PSI">
    <dif>
      Trankvile <ref tip="super" cel="ten.el0i">elteni</ref>, <ref tip="super" cel="persis.0i">persisti</ref>, esti <tld/>a:
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>Dio longe <tld/>as sed</ind> severe rekompencas
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="hu">Isten nem ver bottal</trd>
        <trd lng="ru">
         <ind>&c_B;&c_o;&c_g; &c_d;&c_o;&c_l;&c_g;&c_o; &c_zh;&c_d;&c_jo;&c_t;,</ind>
         &c_d;&c_a; &c_b;&c_o;&c_l;&c_mol;&c_n;&c_o; &c_b;&c_mol;&c_jo;&c_t;
        </trd>
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="esper.0i">esperi</ref>,
    <ref cel="indulg.0i">indulgi</ref>.
  </refgrp>
  <trdgrp lng="be">
  <trd>&c_c;&c_ja;&c_r;&c_p;&c_je;&c_c;&c_mol;</trd>,
  <trd>&c_b;&c_y;&c_c;&c_mol;
  <ind>&c_c;&c_ja;&c_r;&c_p;&c_l;&c_ib;&c_v;&c_y;&c_m;</ind></trd>,
  <trd>&c_s;&c_t;&c_a;&c_v;&c_ib;&c_c;&c_c;&c_a; &c_z;
  <ind>&c_c;&c_ja;&c_r;&c_p;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_ju;</ind></trd>
  </trdgrp>
  <trd lng="br">pasianti&ntilde;</trd>
  <trdgrp lng="ca">
    <trd>ser pacient</trd>,
    <trd>esperar</trd>
  </trdgrp>
  <trd lng="de">sich <ind>gedulden</ind></trd>
  <trd lng="es">ser paciente</trd>
  <trd lng="fr">patienter</trd>
  <trd lng="hu">t&uuml;relmes<klr tip="amb">
  (ige)</klr></trd>
  <trd lng="id">ber<ind>sabar</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x6211;&#x6162;&#x3059;&#x308B;  <pr>&#x304C;&#x307E;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x8F9B;&#x62B1;&#x3059;&#x308B;  <pr>&#x3057;&#x3093;&#x307C;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x6839;&#x6C17;&#x3088;&#x304F;&#x5F85;&#x3064;  <pr>&#x3053;&#x3093;&#x304D;&#x3088;&#x304F;&#x307E;&#x3064;</pr></trd>
  </trdgrp>
  <trd lng="nl">geduld hebben</trd>
  <trd lng="no">v&aelig;re <ind>t&aring;dmodig</ind></trd>
  <trd lng="pl">mie&cacute; <ind>cierpliwo&sacute;&cacute;</ind></trd>
  <trdgrp lng="ru">
  <trd>&c_t;&c_je;&c_r;&c_p;&c_je;&c_t;&c_mol;</trd>,
  <trd>&c_o;&c_t;&c_n;&c_o;&c_s;&c_i;&c_t;&c_mol;&c_s;&c_ja; &c_s;
  <ind>&c_t;&c_je;&c_r;&c_p;&c_je;&c_n;&c_i;&c_je;&c_m;</ind></trd>
  </trdgrp>
  <trd lng="sk">by&tcaron; trpezliv&yacute;</trd>
  <trd lng="cs">b&yacute;t trp&ecaron;liv&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_je;&c_r;&#x43F;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_i;&c_s;&#x44F; &c_z; &c_t;&c_je;&c_r;&#x43F;&c_ib;&#x43D;&#x43D;&#x44F;&#x43C;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_je;&#x43D;&#x43E;&c_s;&c_i;&c_t;&c_i; (&#x43C;&c_u;&#x43A;&c_u;</trd>,
    <trd>&c_v;&c_i;&#x43F;&c_r;&#x43E;&c_b;&c_u;&c_v;&c_a;&#x43D;&#x43D;&#x44F;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7B49;&#x4E00;&#x4F1A; <pr>d&#x11B;ngy&#x12B;hu&#xEC;</pr></trd>,
    <trd>&#x7B49;&#x4E00;&#x6703; <pr>d&#x11B;ngy&#x12B;hu&#xEC;</pr></trd>,
    <trd>&#x5FCD;&#x8010; <pr>r&#x11B;nn&#xE0;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pacien.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="pacien.mal0a.PSI">
    <dif>
      Ne havanta, perdinta la <tld/>on:
      <ekz>
        la princo mal<tld/>e rigardis al la maldekstra flanko, de kie li
        vane atendis la alvenon de Mentezufis
        <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 18</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_c;&c_ja;&c_r;&c_p;&c_l;&c_ib;&c_v;&c_y;</trd>
  <trd lng="bg">&c_n;&c_je;&c_t;&c_malmol;&c_r;&c_p;&c_je;&c_l;&c_i;&c_v;</trd>
  <trd lng="br">dibasiant</trd>
  <trd lng="ca">impacient</trd>
  <trd lng="de">ungeduldig</trd>
  <trd lng="en">impatient</trd>
  <trd lng="es">impaciente</trd>
  <trd lng="fr">impatient</trd>
  <trd lng="hu">t&uuml;relmetlen</trd>
  <trd lng="id">tidak <ind>sabar</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x6027;&#x6025;&#x306A;  <pr>&#x305B;&#x3044;&#x304D;&#x3085;&#x3046;&#x306A;</pr></trd>,
    <trd>&#x77ED;&#x6C17;&#x306A;  <pr>&#x305F;&#x3093;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x305B;&#x3063;&#x304B;&#x3061;&#x306A;</trd>
  </trdgrp>
  <trd lng="nl">ongeduldig</trd>
  <trd lng="no">ut&aring;lmodig</trd>
  <trd lng="pl">niecierpliwy</trd>
  <trd lng="ru">&c_n;&c_je;&c_t;&c_je;&c_r;&c_p;&c_je;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>nedo&ccaron;kav&yacute;</trd>,
    <trd>netrpezliv&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_je;&c_t;&c_je;&c_r;&#x43F;&#x43B;&#x44F;&c_ch;&c_i;&c_j;</trd>,
    <trd>&#x43D;&c_je;&c_t;&c_je;&c_r;&#x43F;&c_je;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4E0D;&#x8010;&#x70E6; <pr>b&#xF9;n&#xE0;if&#xE1;n</pr></trd>,
    <trd>&#x4E0D;&#x8010;&#x7169; <pr>b&#xF9;n&#xE0;if&#xE1;n</pr></trd>,
    <trd>&#x5FC3;&#x5207; <pr>x&#x12B;nqi&#xE8;</pr></trd>,
    <trd>&#x8E81; <pr>z&#xE0;o</pr></trd>,
    <trd>&#x5FC3;&#x6025; <pr>x&#x12B;nj&#xED;</pr></trd>,
    <trd>&#x6025;&#x543C;&#x543C; <pr>j&#xED;h&#x1D2;uh&#x1D2;u</pr></trd>,
    <trd>&#x6081; <pr>ju&#xE0;n</pr></trd>,
    <trd>&#x7126;&#x8E81; <pr>ji&#x101;oz&#xE0;o</pr></trd>,
    <trd>&#x7334;&#x6025; <pr>h&#xF3;uj&#xED;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pacien.mal0o">
  <kap>mal<tld/>o</kap>
  <snc mrk="pacien.mal0o.PSI">
    <dif>
      Malkapablo por longe elteni sen reago:
      <ekz>
        la homo konsistas el mal<tld/>o
        <fnt>
          <aut>trad. I. Chiussi</aut>,
          <vrk>La Nobla Korano</vrk>,
          <lok>1970</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_c;&c_ja;&c_r;&c_p;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_n;&c_je;&c_c;&c_ja;&c_r;&c_p;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">Ungeduld</trd>
  <trd lng="es">impaciencia</trd>
  <trd lng="fr">impatience</trd>
  <trd lng="id">ketidak<ind>sabar</ind>an</trd>
  <trdgrp lng="ja">
    <trd>&#x6027;&#x6025;  <pr>&#x305B;&#x3044;&#x304D;&#x3085;&#x3046;</pr></trd>,
    <trd>&#x77ED;&#x6C17;  <pr>&#x305F;&#x3093;&#x304D;</pr></trd>,
    <trd>&#x305B;&#x3063;&#x304B;&#x3061;</trd>,
    <trd>&#x7126;&#x71E5;  <pr>&#x3057;&#x3087;&#x3046;&#x305D;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">ongeduld</trd>
  <trd lng="no">ut&aring;lmodighet</trd>
  <trd lng="uk">&#x43D;&c_je;&c_t;&c_je;&c_r;&#x43F;&c_ib;&#x43D;&#x43D;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x4E0D;&#x8010;&#x70E6; <pr>b&#xF9;n&#xE0;if&#xE1;n</pr></trd>,
    <trd>&#x4E0D;&#x8010;&#x7169; <pr>b&#xF9;n&#xE0;if&#xE1;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pacien.sen0igxi">
  <kap>sen<tld/>i&gcirc;i,
  <var><kap>perdi la <tld/>on</kap></var></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="pacien.sen0igxi.PSI">
    <dif>
      Post <tld/>ado ne plu elteni,
      <ref tip="vid" cel="incit.0igxi">inciti&gcirc;i</ref>:
      <ekz>
        pri la hejma vivo li sen<tld/>i&gcirc;is
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_mol;&c_n;&c_je;&c_c;&c_ja;&c_r;&c_p;&c_l;&c_ib;&c_v;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_t;&c_r;&c_a;&c_c;&c_ib;&c_c;&c_mol; <ind>&c_c;&c_ja;&c_r;&c_p;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</ind></trd>
  </trdgrp>
    <trdgrp lng="de">
    <trd>ungeduldig werden</trd>,
    <trd>die <ind>Geduld</ind> verlieren</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>impatienter</ind></trd>,
    <trd>perdre <ind>patience</ind></trd>
  </trdgrp>
   <trd lng="nl">ongeduldig worden</trd>
  <trdgrp lng="zh">
    <trd>&#x4E0D;&#x8010;&#x70E6;&#x4E86; <pr>b&#xF9;n&#xE0;if&#xE1;nli&#x1CE;o</pr></trd>,
    <trd>&#x4E0D;&#x8010;&#x7169;&#x4E86; <pr>b&#xF9;n&#xE0;if&#xE1;nli&#x1CE;o</pr></trd>,
    <trd>&#x53D1;&#x813E;&#x6C14; <pr>f&#x101;p&#xED;q&#xEC;</pr></trd>,
    <trd>&#x767C;&#x813E;&#x6C23; <pr>f&#x101;p&#xED;q&#xEC;</pr></trd>
  </trdgrp>
</drv>
