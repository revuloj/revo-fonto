<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>akcipitr</rad>/o
</kap>
<drv mrk="akcipi.0o">
<kap><ofc>*</ofc><tld/>o</kap>
<uzo tip="fak">ZOO</uzo>
<snc>
<dif>
Genro el familio <ref tip="super" cel="akcipi.0edoj">akcipitredoj</ref>
(<trd lng="la">Accipiter</trd>).
</dif>
</snc>
<trd lng="cs">jestr&aacute;b</trd>
<trdgrp lng="de"><trd>Habicht</trd>, <trd>Sperber</trd></trdgrp>
<trd lng="en">hawk</trd>
<trd lng="hu">h&eacute;ja</trd>
<trdgrp lng="pl"><trd>jastrz&aogonek;b</trd>, <trd>krogulec</trd></trdgrp>
<trdgrp lng="ru">
  <trd>&#1103;&#1089;&#1090;&#1088;&#1077;&#1073;</trd>,  
  <trd>&c_t;&c_je;&c_r;&c_je;&c_v;&c_ja;&c_t;&c_n;&c_i;&c_k;</trd>,  
  <trd>&c_p;&c_je;&c_r;&c_je;&c_p;&c_je;&c_l;&c_ja;&c_t;&c_n;&c_i;&c_k;</trd>
</trdgrp>
</drv>
<drv mrk="akcipi.0edoj">
<kap><tld/>edoj</kap>
<uzo tip="fak">ZOO</uzo>
<snc>
<dif>
Familio el la ordo <ref tip="super" cel="bird.rabo0oj">rabobirdoj</ref> 
(<trd lng="la">Accipitridae</trd>).
</dif>
<refgrp tip="sub">
<ref cel="agl.0o">aglo</ref>,
<ref cel="akcipi.0o"><tld/>o</ref>,
<ref cel="bute.0o">buteo</ref>,
<ref cel="milv.0o">milvo</ref>.
</refgrp>
</snc>
<trd lng="de">Habichtartige</trd>
</drv>
</art>
