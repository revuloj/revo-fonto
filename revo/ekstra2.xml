<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>ekstradici</rad>/i
</kap>
<drv mrk="ekstra2.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">JUR</uzo>  
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Transdoni fu&gcirc;intan kriminton al
      la kompetentaj juraj instancoj de alia lando:
      <ekz>
        kelkaj landoj rifuzas la <ind><tld/>o</ind>n en &ccirc;iaj okazoj.
        <trd lng="de">Auslieferung</trd>
        <trd lng="es">extradici&oacute;n</trd>
        <trd lng="fr">extradition</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">ausliefern</trd>
  <trd lng="es">extraditar</trd>
  <trd lng="fr">extrader</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_d;&c_a;&c_t;&c_mol; <klr>(&c_p;&c_r;&c_je;&c_s;&c_t;&c_u;&c_p;&c_n;&c_i;&c_k;&c_a; &c_d;&c_r;&c_u;&c_g;&c_o;&c_m;&c_u; &c_g;&c_o;&c_s;&c_u;&c_d;&c_a;&c_r;&c_s;&c_t;&c_v;&c_u;)</klr></trd>,
    <trd>&c_p;&c_o;&c_d;&c_v;&c_je;&c_r;&c_g;&c_n;&c_u;&c_t;&c_mol; &c_e;&c_k;&c_s;&c_t;&c_r;&c_a;&c_d;&c_i;&c_c;&c_i;&c_i;</trd>
  </trdgrp>
</drv>
</art>

