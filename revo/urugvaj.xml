<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>8</ofc>
     <rad>Urugvaj</rad>/o <fnt><bib>PIV1</bib></fnt></kap>

<drv mrk="urugvaj.U0o">
  <kap><ofc>8</ofc><tld/>o
   <fnt><bib>RLN</bib></fnt>
  </kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      Sudamerika <ref tip="lst" lst="voko:&scirc;tatoj"
      cel="sxtat.0o">&scirc;tato</ref>, 
      limanta al <ref tip="vid" cel="brazil.B0o">Brazilo</ref>,
      <ref tip="vid" cel="argentin.A0o">Argentino</ref> kaj al 
      <ref tip="vid" cel="atlant.A0o">Atlantiko</ref>:
      <ekz>
        la unua vira Futbala Mondpokalo organizata de FIFA okazis en
        julio de 1930 en Montevideo, &ccirc;efurbo de <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=138176">Futbala
          Mondpokalo 1930</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la malpermeso de la grupo <nom>Mercosur</nom> &ndash; kiu inkluzivas
        anka&ubreve; Brazilon, Paragvajon 
        kaj <tld/>on &ndash; estas la plej lasta flami&gcirc;o en la disputo inter
        Argentino kaj Britio pri la insuloj
        <fnt>
          <bib>Monato</bib>
          <aut>Lenio Marobin</aut>
          <vrk><url
          ref="&Monato;/2012/010699.php">Neakcepteblaj kaj
          nepravigeblaj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_U;&c_r;&c_u;&c_g;&c_v;&c_a;&c_j;</trd>
  <trd lng="ca">Uruguai</trd>
  <trd lng="cs">Uruguay</trd>
  <trd lng="da">Uruguay</trd>
  <trd lng="de">Uruguay</trd>
  <trd lng="en">Uruguay</trd>
  <trd lng="es">Uruguay</trd>
  <trd lng="fi">Uruguay</trd>
  <trd lng="fr">Uruguay</trd>
  <trd lng="hu">Uruguay</trd>
  <trd lng="id">Uruguay</trd>
  <trd lng="it">Uruguay</trd>
  <trd lng="ja">&#12454;&#12523;&#12464;&#12450;&#12452; [<ind>&#12358;&#12427;&#12368;&#12354;&#12356;</ind>]</trd>
  <trd lng="nl">Uruguay</trd>
  <trd lng="pl">Urugwaj</trd>
  <trd lng="pt">Uruguai</trd>
  <trd lng="ru">&c_U;&c_r;&c_u;&c_g;&c_v;&c_a;&c_j;</trd>
  <trd lng="sk">Uruguaj</trd>
  <trd lng="sl">Urugvaj</trd>
  <trd lng="sv">Uruguay</trd>
  <trd lng="tp">ma Ulukawi</trd>
  <trd lng="tr">Uruguay</trd>
  <trd lng="uk">&c_U;&c_r;&c_u;&c_g;&c_v;&c_a;&c_j;</trd>
</drv>


<drv mrk="urugvaj.0a">
  <kap><tld lit="u"/>a</kap>
  <snc>
    <dif>
    Rilata al la &scirc;tato <tld/>o:
      <ekz>
        Esperanto en Montevideo &#8208; la landa asocio <tld lit="u"/>a
        fondi&gcirc;is en 1924 
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref="https://eo.wikipedia.org/?curid=31215">Montevideo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Enrique Legrand</nom> <klr>[&#8230;]</klr> estis <tld lit="u"/>a
        sciencisto, inventisto, esperantisto 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=174001">Enrique
          Legrand</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">Uruguayan<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="es">uruguayo</trd>
  <trdgrp lng="fr"> 
    <trd>uruguayen</trd>
    <trd>uruguayenne<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#12454;&#12523;&#12464;&#12450;&#12452;&#12398; [<ind>&#12358;&#12427;&#12368;&#12354;&#12356;&#12398;</ind>]</trd>
  <trd lng="nl">Uruguayaans</trd>
  <trd lng="tp">pi ma Ulukawi</trd>
</drv>


<drv mrk="urugvaj.0ano">
  <kap><tld lit="u"/>ano,
  <var><kap><tld lit="u"/>anino</kap></var></kap>
  <snc>
    <dif>
    Civitano de <tld/>o:
      <ekz>
        <nom>Anna Held</nom> &#8208; &scirc;ia edzo estis <tld lit="u"/>ano
        <nom>Maximo Carrera</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=606011">Anna
          Held</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">Uruguayan<klr tip="ind"> (person)</klr></trd>
  <trd lng="es">uruguayo</trd>
  <trdgrp lng="fr"> 
    <trd>Uruguayen</trd>
    <trd>Uruguayenne<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#12454;&#12523;&#12464;&#12450;&#12452;&#20154; [<ind>&#12358;&#12427;&#12368;&#12354;&#12356;&#12376;&#12435;</ind>]</trd>
  <trd lng="nl">Uruguayaan</trd>
  <trd lng="tp">jan pi ma Ulukawi</trd>
</drv>

</art>

