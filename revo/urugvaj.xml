<vortaro>
<art mrk="$Id$">
<kap><ofc>8</ofc>
     <rad>Urugvaj</rad>/o <fnt><bib>PIV1</bib></fnt></kap>

<drv mrk="urugvaj.U0o">
  <kap><ofc>8</ofc><tld/>o
   <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/aktoj/aktoj2/8oa.html"> 8a
Oficiala Aldono</url></vrk>, <lok>II.C.1</lok></fnt>
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>
    <fnt><bib>Viki</bib>, <lok><url 
        ref="http://eo.wikipedia.org/wiki/Urugvajo">Urugvajo</url></lok></fnt>
  </kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      Sudamerika <ref tip="super" cel="sxtat.0o">&scirc;tato</ref>, 
      limanta al <ref tip="vid" cel="brazil.B0o">Brazilo</ref>,
      <ref tip="vid" cel="argentin.A0o">Argentino</ref> kaj al 
      <ref tip="vid" cel="atlant.A0o">Atlantiko</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_U;&c_r;&c_u;&c_g;&c_v;&c_a;&c_j;</trd>
  <trd lng="ca">Uruguai</trd>
  <trd lng="cs">Uruguay</trd>
  <trd lng="da">Uruguay</trd>
  <trd lng="de">Uruguay</trd>
  <trd lng="en">Uruguay</trd>
  <trd lng="es">Uruguay</trd>
  <trd lng="fi">Uruguay</trd>
  <trd lng="fr">Uruguay</trd>
  <trd lng="hu">Uruguay</trd>
  <trd lng="id">Uruguay</trd>
  <trd lng="it">Uruguay</trd>
  <trd lng="ja">&#12454;&#12523;&#12464;&#12450;&#12452;</trd>
  <trd lng="nl">Uruguay</trd>
  <trd lng="pl">Urugwaj</trd>
  <trd lng="pt">Uruguai</trd>
  <trd lng="ru">&c_U;&c_r;&c_u;&c_g;&c_v;&c_a;&c_j;</trd>
  <trd lng="sk">Uruguaj</trd>
  <trd lng="sl">Urugvaj</trd>
  <trd lng="sv">Uruguay</trd>
  <trd lng="tr">Uruguay</trd>
  <trd lng="uk">&c_U;&c_r;&c_u;&c_g;&c_v;&c_a;&c_j;</trd>
</drv>

<drv mrk="urugvaj.0ano">
  <kap><tld lit="u"/>ano,
  <var><kap><tld lit="u"/>anino</kap></var></kap>
  <snc>
    <dif>
    Civitano, civitanino de Urugvajo.
    </dif>
  </snc>
  <trdgrp lng="fr"> 
    <trd>Uruguayen</trd>
    <trd>Uruguayenne<klr> (subst.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="urugvaj.0a">
  <kap><tld lit="u"/>a</kap>
  <snc>
    <dif>
    Rilata al la &scirc;tato Urugvajo.
    </dif>
  </snc>
  <trdgrp lng="fr"> 
    <trd>uruguayen</trd>
    <trd>uruguayenne<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>

</art>

