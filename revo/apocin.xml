<art mrk="$Id: apocin.xml,v 1.13 2022/11/30 17:29:14 revo Exp $">
<kap>
  <rad>apocinac</rad>/oj
</kap>
<drv mrk="apocin.0oj">
  <kap>
    <tld/>oj
  </kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref>  de <ref
      tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Apocynaceae</trd>):
      <ekz>
        la <tld/>oj <klr>[&#8230;]</klr> havas &ccirc;. 164 genrojn kaj 1500 speciojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19752">Apocinacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
       <ref cel="vink.0o">vinko</ref>,
       <ref cel="neri.0o">nerio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_t;&c_r;&c_o;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  <trd lng="de">Hundsgiftgew&auml;chse</trd>
  <trd lng="fr">Apocynac&eacute;es</trd>
  <trdgrp lng="hu">
    <trd>met&eacute;ngf&eacute;l&eacute;k</trd>,
    <trd>b&ouml;rv&eacute;nyf&eacute;l&eacute;k</trd>,
    <trd>t&eacute;liz&ouml;ldf&eacute;l&eacute;k</trd></trdgrp>
  <trd lng="ru">&c_k;&c_u;&c_t;&c_r;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&c_b;&c_a;&c_r;&c_v;&c_ib;&#x43D;&#x43A;&#x43E;&c_v;&c_ib;</trd>
</drv>
</art>
