<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc> <rad>&gcirc;entil</rad>/a</kap>
<drv mrk="gxenti.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Observanta la sociajn decojn, bonmaniere afabla:
      <ekz>
        <tld/>a homo<fnt>B</fnt>, konduto, ago;
      </ekz>
      <ekz>&ccirc;e tro <tld/>a ekstero mankas sincero<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        kiam malri&ccirc;a virino petos de vi trinki, vi donos &gcirc;in al
        &scirc;i <tld/>e
        <fnt><bib>F</bib>, <lok>&FE; 17</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_je;&c_t;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_ch;&c_c;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_d;&c_a;&c_l;&c_ib;&c_k;&c_a;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">zdvo&rcaron;il&yacute;</trd>
  <trd lng="de">h&ouml;flich</trd>
  <trd lng="en">polite</trd>
  <trd lng="es">gentil</trd>
  <trdgrp lng="fr">
    <trd>poli<klr tip="ind"> (courtois)</klr></trd>,
    <trd>courtois</trd>,
    <trd>biens&eacute;ant</trd>
  </trdgrp>
  <trd lng="nl">beleefd</trd>
  <trd lng="hu">udvarias</trd>
  <trd lng="it">gentile</trd>
  <trd lng="pt">gentil</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_zh;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_u;&c_ch;&c_t;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="vo">pl&uuml;tik</trd>
</drv>

<drv mrk="gxenti.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Konduto konforma al socia deco:
      <ekz>
        la afableco estas eco kunnaskita, la <ind><tld/>eco</ind>
        akirita
        <fnt><bib>VdE</bib>, <lok></lok></fnt>;
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_je;&c_t;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_ch;&c_c;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_h;&c_a;&c_v;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="es">gentileza</trd>
  <trdgrp lng="fr">
    <trd>biens&eacute;ance</trd>,
    <trd>courtoisie</trd>,
    <trd>politesse</trd>
  </trdgrp>
  <trd lng="en">politeness</trd>
  <trd lng="it">gentilezza</trd>
  <trd lng="pt">gentileza</trd>
</drv>

<drv mrk="gxenti.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Tute ne <tld/>a:
      <ekz>
        mal<tld/>a kaj fiera knabino
        <fnt><bib>F</bib>, <lok>&FE; 19</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="arogan.0a">aroganta</ref>,
      <ref cel="impert.0a">impertinenta</ref>,
      <ref cel="krud.0a">kruda</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_v;&c_je;&c_t;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_a;&c_ch;&c_c;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_g;&c_r;&c_u;&c_b;&c_y;</trd>
  </trdgrp>
  <trd lng="de">unh&ouml;flich</trd>
  <trdgrp lng="en">
    <trd>impolite</trd>,
    <trd>rude</trd>
  </trdgrp>
  <trd lng="es">grosero</trd>
  <trdgrp lng="fr">
    <trd>impoli</trd>,
    <trd>discourtois</trd>,
    <trd>mals&eacute;ant</trd>
  </trdgrp>
  <trd lng="hu">udvariatlan</trd>
  <trdgrp lng="it">
    <trd>scortese</trd>,
    <trd>maleducato</trd>
  </trdgrp>
  <trd lng="nl">onbeleefd</trd>
  <trdgrp lng="pt">
    <trd>descort&ecirc;s</trd>,
    <trd>mal-educado</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_v;&c_je;&c_zh;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_l;&c_ju;&c_b;&c_je;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_g;&c_r;&c_u;&c_b;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="gxenti.0acxa">
  <kap><tld/>a&ccirc;a</kap>
  <snc>
    <dif>
      Flate tro <tld/>a.
    </dif>
  </snc>
  <trd lng="de">schmierig <klr>(Verhalten)</klr></trd>
  <trd lng="fr">obs&eacute;quieux</trd>
  <trd lng="hu">udvariaskod&oacute;</trd>
</drv>

<drv mrk="gxenti.mal0ulo">
  <kap>mal<tld/>ulo</kap>
  <snc>
    <dif>
      Homo malafable kruda:
      <ekz>
        la mal<tld/>ulino <klr>[respondis]</klr>, el&jcirc;etante unu
        serpenton
        kaj unu ranon
        <fnt><bib>F</bib>, <lok>&FE; 21</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_u;&c_b;&c_ib;&c_ja;&c_n;</trd>
  <trd lng="es">grosero</trd>
  <trd lng="fr">malappris</trd>
  <trd lng="hu">faragatlan ember</trd>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_b;&c_i;&c_ja;&c_n;</trd>
</drv>

<drv mrk="gxenti.0homo">
  <kap><tld/>homo</kap>
  <uzo tip="stl">ARK</uzo>
  <fnt><bib>PV</bib></fnt>
  <snc>
    <dif>
      <ref tip="dif" cel="gxentl.0o">&Gcirc;entlemano</ref>.
    </dif>
    <rim>
      Tiu vorto estas evidenta pa&ubreve;sa&jcirc;o de la franca, ne
      tute konforma al la Esperanta vortfarado kaj strange uzanta la
      elementon
      "homo" anstata&ubreve; "viro".
    </rim>
  </snc>
  <trd lng="be">&c_d;&c_zh;&c_e;&c_n;&c_t;&c_l;&c_mol;&c_m;&c_e;&c_n;</trd>
  <trd lng="es">caballero</trd>
  <trd lng="en">gentleman</trd>
  <trdgrp lng="fr">
    <trd>gentilhomme<klr tip="ind"> (homme de bonne
    &eacute;ducation)</klr></trd>,
    <trd>gentleman<klr tip="ind"> (homme de bonne
    &eacute;ducation)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&uacute;riember<klr tip="ind"> (finom modor&uacute;)</klr></trd>,
    <trd>gentleman</trd>
  </trdgrp>
  <trd lng="it">gentiluomo</trd>
  <trd lng="nl">gentleman</trd>
  <trdgrp lng="pt">
    <trd>cavalheiro</trd>,
    <trd>gentil-homem</trd>
  </trdgrp>
  <trd
  lng="ru">&c_d;&c_zh;&c_je;&c_n;&c_t;&c_l;&c_mol;&c_m;&c_je;&c_n;</trd>
</drv>
</art>
