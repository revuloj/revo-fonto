<drv mrk="gxenti.0umi">
  <kap><tld/>umi</kap>
  <snc mrk="gxenti.0umi.KOMUNE">
    <dif>
      Atente, afekte sin teni la&ubreve; etiketo:
      <ekz>
        <ctl>kleraj sinjoroj kaj graciaj sinjorinoj,</ctl>
        li diris <klr>...</klr> (li &ccirc;iam <tld/>umis tiel&#xFF09;
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_n;&c_je;&c_zh;&c_y;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_f;&c_ib;&c_g;&c_u;&c_r;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="fr">faire des <ind>mani&egrave;res</ind></trd>
<trdgrp lng="id">
  <trd><baz>ramah</baz>beramah-ramah</trd>,
  <trd><baz>sikap</baz>bersikap sopan</trd>,
  <trd><baz>sopan</baz>bersopan-sopan</trd>
</trdgrp>
</drv>

