<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kaland1.xml,v 1.24 2023/12/02 21:20:04 revo Exp $">
<kap>
   <rad>kalandr</rad>/i
   <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="kaland1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Glatigi tola&jcirc;ojn, &scirc;tofon, paperon per specialaj
      rulcilindroj, por doni al ili brilecon.
      <ekz>
        vesto el bruna <tld/>ita indieno
        <fnt>
          <vrk><url ref="http://miaj-tradukoj.e-monsite.com/pages/essai/16-pri-nova-mastro-kaj-pri-lia-kunvivantaro.html">La Kabano de Onklo Tomo</url></vrk>
          <lok>16. Pri nova Mastro kaj pri lia kunvivantaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la papero estis tiel glata kiel <tld/>ita kanvaso
        <fnt>
          <aut>Ingegerd Granat</aut>
          <vrk><url ref="https://esperanto-gbg.org/oraklubo/Ora_Jxurnalo_n-ro_61.pdf">Dekmil ru&gcirc;ajn rozojn</url></vrk>
          <lok>Ora &Jcirc;urnalo, n-ro 61, Aprilo 2023, p.14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>hladit</trd>,
    <trd>kalandrovat</trd>,
    <trd>le&scaron;tit</trd>,
    <trd>vyrovn&aacute;vat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>kalandern</trd>,
    <trd>kalandrieren</trd>,
    <trd>mangeln <klr>(W&auml;sche)</klr></trd>,
    <trd>rollen <klr>(W&auml;sche)</klr></trd>
  </trdgrp>
  <trd lng="en">mangle</trd>
  <trdgrp lng="es">
     <trd>satinar</trd>,
     <trd>calandrar</trd>
   </trdgrp>
  <trdgrp lng="fi">
     <trd>mankeloida</trd>,
     <trd>kalanteroida <klr> (paperon en fabriko)</klr></trd>
   </trdgrp>
  <trdgrp lng="fr">
     <trd>calandrer</trd>,
     <trd>lustrer<klr tip="ind"> (&agrave; la calandre)</klr></trd>
   </trdgrp>
  <trdgrp lng="hu">
     <trd>m&aacute;ngorol</trd>,
     <trd>kalanderez <klr tip="ind">(sz&ouml;vetet)</klr></trd>
   </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5149;&#x6CA2;&#x6A5F;&#x306B;&#x304B;&#x3051;&#x308B;  <pr>&#x3053;&#x3046;&#x305F;&#x304F;&#x304D;&#x306B;&#x304B;&#x3051;&#x308B;</pr></trd>,
    <trd>&#x3064;&#x3084;&#x51FA;&#x3057;&#x52A0;&#x5DE5;&#x3059;&#x308B;  <pr>&#x3064;&#x3084;&#x3060;&#x3057;&#x304B;&#x3053;&#x3046;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">kalanderen</trd>
  <trd lng="pl">kalandrowa&cacute;</trd>
  <trd lng="pt">calandrar</trd>
  <trdgrp lng="ru">
     <trd>&c_k;&c_a;&c_t;&c_a;&c_t;&c_mol;
<klr>(&c_b;&c_je;&c_l;&c_mol;&c_jo;)</klr></trd>,
     <trd>&c_l;&c_o;&c_shch;&c_i;&c_t;&c_mol;
<klr>(&c_b;&c_u;&c_m;&c_a;&c_g;&c_u;)</klr></trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>hladi&tcaron;</trd>,
    <trd>kalandrova&tcaron;</trd>,
    <trd>le&scaron;ti&tcaron;</trd>,
    <trd>mang&lcaron;ova&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">mangla</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_ch;&c_a;&c_t;&c_i; &c_b;&c_ib;&c_l;&c_i;&c_z;&c_n;&c_u;</trd>,
    <trd>&c_m;&c_a;&c_gu;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_a;&c_n;&c_d;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_l;&c_o;&c_shch;&c_i;&c_t;&c_i; &c_p;&c_a;&c_p;&c_ib;&c_r;</trd>
  </trdgrp>
  <trd lng="zh">&#x7811; <pr>y&#xE0;</pr></trd>
</drv>

<drv mrk="kaland1.0o">
   <kap><tld/>o</kap>
   <snc>
     <dif>
       Ago <tld/>i.
     </dif>
   </snc>
   <refgrp tip="hom">
     Ne konfuzu kun:
     <ref cel="kaland3.0o">kalandro</ref> (birdo),
     <ref cel="kaland.0o">kalandr(a)o</ref> (insekto).
   </refgrp>
   <trd lng="de">Kalandrieren</trd>
   <trd lng="en">mangle</trd>
   <trd lng="fi">mankelointi</trd>
   <trdgrp lng="fr">
     <trd>calandrage</trd>,
     <trd>lustrage<klr tip="ind"> (&agrave; la calandre)</klr></trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>m&aacute;ngorl&aacute;s</trd>,
     <trd>kalanderez&eacute;s <klr tip="ind">(sz&ouml;vet&eacute;)</klr></trd>
   </trdgrp>
   <trd lng="ja">&#x30AF;&#x30ED;&#x30A8;&#x30EA;&#x30B3;&#x30A6;&#x30C6;&#x30F3;&#x30B7;</trd>
  <trd lng="nl">kalanderen<klr tip="ind"> (zn.)</klr></trd>
   <trd lng="pl">kalandrowanie</trd>
  <trd lng="sk">mang&lcaron;ovanie bielizne</trd>
  <trdgrp lng="cs">
    <trd>kalandrov&aacute;n&iacute; pr&aacute;dla</trd>,
    <trd>mandlov&aacute;n&iacute; pr&aacute;dla</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kalandrieren</trd>,
    <trd>Mangeln <klr>(W&auml;sche)</klr></trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_t;&c_je;&c_p;&c_o;&c_v;&c_i;&c_j; &c_zh;&c_a;&c_j;&c_v;&c_o;&c_r;&c_o;&c_n;&c_o;&c_k; &c_s;&c_p;&c_ib;&c_v;&c_u;&c_ch;&c_i;&c_j;</trd>
</drv>

<drv mrk="kaland1.0ilo">
   <kap><tld/>ilo</kap>
   <uzo tip="fak">TEK</uzo>
   <snc>
     <dif>
       Ma&scirc;ino por glatigi kaj brilantigi &scirc;tofojn, glaceigi paperon:
      <ekz>
        en la urbo dum 15&#x202F;-&#x202F;17<klr>[a]</klr> jarcentoj estis du urboplacoj,
        tri pre&gcirc;ejoj, urbodomo, urba bandomo (banejo), <klr>[&#x2026;]</klr> domo en
        kiu oni preparis drapojn, <tld/>ilo (rulpremilo) kun la loko kie oni blankigis la teksa&jcirc;ojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=196664">Strzy&zdot;&oacute;w</url></lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trd lng="cs">kalndr</trd>
  <trdgrp lng="de">
    <trd>Kalander</trd>,
    <trd>W&auml;schemangel</trd>,
    <trd>Mangel <klr>(W&auml;sche)</klr></trd>,
    <trd>Rolle <klr>(W&auml;sche)</klr></trd>
  </trdgrp>
  <trd lng="en">mangle</trd>
  <trdgrp lng="fi">
     <trd>mankeli</trd>,
     <trd>kalanteri <klr> (en paperfabriko)</klr></trd>
   </trdgrp>
  <trd lng="fr">calandre<klr tip="ind"> (machine pour
   lustrer)</klr></trd>
  <trdgrp lng="hu">
     <trd>m&aacute;ngorl&oacute;(g&eacute;p)</trd>,
     <trd>kalander <klr tip="ind">(sz&ouml;vethez)</klr></trd>
   </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30AB;&#x30EC;&#x30F3;&#x30C0;&#x30FC;  <pr>&#x304B;&#x308C;&#x3093;&#x3060;&#x30FC;</pr></trd>,
    <trd>&#x5149;&#x6CA2;&#x6A5F;  <pr>&#x3053;&#x3046;&#x305F;&#x304F;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="nl">kalander<klr tip="ind"> (toestel)</klr></trd>
  <trd lng="pl">kalandra</trd>
  <trd lng="ru">&c_k;&c_a;&c_l;&c_a;&c_n;&c_d;&c_r;</trd>
  <trd lng="sk">mange&lcaron;</trd>
  <trd lng="sv">mangel</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_u;&c_b;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_gu;&c_l;&c_ib;&c_v;&c_n;&c_i;&c_c;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_a;&c_n;&c_d;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7811;&#x5149;&#x673A; <pr>y&#xE0;gu&#x101;ngj&#x12B;</pr></trd>,
    <trd>&#x7811;&#x5149;&#x8F25; <pr>y&#xE0;gu&#x101;ngg&#x1D4;n</pr></trd>
  </trdgrp>
</drv>
</art>
