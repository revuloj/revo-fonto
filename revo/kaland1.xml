<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kalandr</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="kaland1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Glatigi tola&jcirc;ojn, &scirc;tofon, paperon per specialaj 
      rulcilindroj, por doni al ili brilecon.
    </dif>
  </snc>
  <trd lng="de">mangeln</trd>
  <trd lng="en">mangle</trd>
  <trdgrp lng="fr">
    <trd>calandrer</trd>,
    <trd>lustrer<klr tip="ind"> (&agrave; la calandre)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>m&aacute;ngorol</trd>,
    <trd>kalanderez <klr tip="ind">(sz&ouml;vetet)</klr></trd>
  </trdgrp>
  <trd lng="nl">kalanderen</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_t;&c_a;&c_t;&c_mol; <klr>(&c_b;&c_je;&c_l;&c_mol;&c_jo;)</klr></trd>,
    <trd>&c_l;&c_o;&c_shch;&c_i;&c_t;&c_mol; <klr>(&c_b;&c_u;&c_m;&c_a;&c_g;&c_u;)</klr></trd>
  </trdgrp>
  <trd lng="sv">mangla</trd>
</drv>

<drv mrk="kaland1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <refgrp tip="hom">
    Ne miksu kun:
    <ref cel="kaland3.0o">kalandro</ref> (birdo),
    <ref cel="kaland.0o">kalandro</ref> (insekto).
  </refgrp>
  <trd lng="de">Mangeln</trd>
  <trd lng="en">mangle</trd>
  <trdgrp lng="fr">
    <trd>calandrage</trd>,
    <trd>lustrage<klr tip="ind"> (&agrave; la calandre)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>m&aacute;ngorl&aacute;s</trd>,
    <trd>kalanderez&eacute;s <klr tip="ind">(sz&ouml;vet&eacute;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="kaland1.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Ma&scirc;ino por glatigi kaj brilantigi &scirc;tofojn, glaceigi paperon:
      <ekz>
        <tld/>iloj estas formitaj el tri 
        <ref tip="prt" cel="cilind.rul0o">rulcilindroj</ref>: 
        du kartonaj kaj unu metala.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Mangel</trd>
  <trd lng="en">mangle</trd>
  <trd lng="fr">calandre<klr tip="ind"> (machine pour lustrer)</klr></trd>
  <trdgrp lng="hu">
    <trd>m&aacute;ngorl&oacute;(g&eacute;p)</trd>,
    <trd>kalander <klr tip="ind">(sz&ouml;vethez)</klr></trd>
  </trdgrp>
  <trd lng="nl">kalander<klr tip="ind"> (toestel)</klr></trd>
  <trd lng="ru">&c_k;&c_a;&c_l;&c_a;&c_n;&c_d;&c_r;</trd>
  <trd lng="sv">mangel</trd>
</drv>
</art>
