<kap><rad>gastrostomi</rad>/o</kap>
<drv mrk="gastro3.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc mrk="gastro3.0o.operacio">
    <dif>
      Operacio por starigi da&ubreve;ran stomakan ha&ubreve;t-aperturon:
      <ekz>
        tra la <tld/>o la paciento nutras <klr>[sin]</klr>
        <fnt>
          <vrk><url ref="https://eo.birmiss.com/sindromo-de-franceschetti-kauzoj-simptomoj-kaj-traktado-de-malsano/">Sindromo De Franceschetti: Ka&ubreve;zoj, Simptomoj Kaj Traktado De Malsano</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gastro4.0o">gastrotomio</ref>,
      <ref cel="laparo">laparotomio</ref>
    </refgrp>
  </snc>
  <trd lng="ca">gastrostomia</trd>
  <trd lng="de">Gastrostomie</trd>
  <trd lng="en">gastrostomy</trd>
  <trd lng="fr">gastrostomie</trd>
  <trdgrp lng="hu">
    <trd>gyomorsipoly k&eacute;pz&eacute;se</trd>,
    <trd>gastrostomia</trd>
  </trdgrp>
  <trd lng="nl">gastrostomie</trd>
  <trd lng="pt">gastrostomia</trd>
  <trd lng="ru">&c_g;&c_a;&c_s;&c_t;&c_r;&c_o;&c_s;&c_t;&c_o;&c_m;&c_i;&c_ja;</trd>
  <trd lng="uk">&#x433;&#x430;&#x441;&#x442;&#x440;&#x43E;&#x441;&#x442;&#x43E;&#x43C;&#x456;&#x44F;</trd>
</drv>
