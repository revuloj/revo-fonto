<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>herb</rad>/o
</kap>
<drv mrk="herb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Mola vegeta&jcirc;o, kies aeraj partoj restas &ccirc;iam verdaj
      kaj mortas &ccirc;iun jaron:
      <ekz>
        tritiko, aveno, rizo, plantago, petroselo, laktuko estas <tld/>oj;
      </ekz>
      <ekz>
        malbona <tld/>o froston ne timas<fnt>Z</fnt>;
      </ekz>
      <ekz>
        medicinaj <tld/>oj;
      </ekz>
      <ekz>
        <tld/>ori&ccirc;aj pa&scirc;tejoj;
      </ekz>
      <ekz>
        <tld/>oman&gcirc;(ant)aj bestoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">tr&aacute;va</trd>
  <trd lng="de">Kraut</trd>
  <trd lng="en">grass</trd>
  <trd lng="fr">herbe</trd>
</drv>
<drv mrk="herb.0acxo">
  <kap><tld/>a&ccirc;o</kap>
  <snc>
    <dif>
      Malbona <tld/>o, malutila por la kulturado. 
    </dif>
  </snc>
  <trd lng="de">Unkraut</trd>
  <trd lng="fr">mauvaise herbe</trd>
  <trd lng="nl">onkruid</trd>
</drv>
<drv mrk="herb.0aro0ejo">
  <kap><tld/>aro, <tld/>ejo</kap>
  <snc>
    <dif>
      Loko, kie kreskas sova&gcirc;aj a&ubreve; kulturitaj pa&scirc;t<tld/>oj. 
    </dif>
  </snc>
  <trd lng="de">Wiese</trd>
  <trd lng="fr">prairie</trd>
</drv>
<drv mrk="herb.0ero">
  <kap><tld/>ero</kap>
  <snc>
    <dif>
      &Ccirc;iu el la trunketoj de <tld/>o. 
    </dif>
  </snc>
</drv>
<drv mrk="herb.hund0o">
  <kap><tld/>herbo</kap>
  <snc>
    <ref tip="dif" cel="agropi.0o">Agropiro</ref>  
  </snc>
</drv>
</art>
