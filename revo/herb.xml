<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>herb</rad>/o
</kap>
<drv mrk="herb.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="herb.0o.KOMUNE">
    <dif>
      Malalta, neligna planto, precipe tia kreskanta sur
      kampo:
      <ekz>
        per herbo oni vin nutros kiel bovojn
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Daniel 4:25</lok>)</fnt>;
      </ekz>
      <ekz>
        kiel herbo, matene &gcirc;i floras kaj &gcirc;ermas, vespere
        &gcirc;i dehaki&gcirc;as kaj seki&gcirc;as
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Psalmaro 90:5</lok>)</fnt>;
      </ekz>
      <ekz>
        venis akridoj kaj skaraboj sennombraj kaj forman&gcirc;is
        la tutan herbon en ilia lando
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Psalmaro 105:34</lok>)</fnt>;
      </ekz>
      <ekz>
        &ccirc;ar kiel floro de herbo li forpasos
        <fnt>(<vrk>Nova testamento</vrk>,
        <lok>Jakobo 1, 11</lok>)</fnt>;
      </ekz>
      <ekz>
        la verda herbo kun la malgrandaj ru&gcirc;aj kaj blankaj
        floroj estis la subpieda tapi&scirc;o
        <fnt>(<aut>Andersen, trad. Zamenhof</aut>: <vrk>Fabeloj,
        Vojkamarado</vrk>)</fnt>.
      </ekz>
    </dif>
    <rim>
      La&ubreve; la tradukoj en fundamento temas precipe
      pri <ref tip="vid" cel="poac.0oj">poacoj</ref> kiel
      grenoj, festuko, elimo ktp. Sed kiel montras la lastaj du
      ekzemploj la fakta uzo estas pli vasta.
    </rim>
    <trdgrp lng="de">
      <trd>Kraut</trd>,
      <trd>Gras</trd>
    </trdgrp>
  </snc>
  <snc mrk="herb.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="sin" cel="plant.nedauxra0o">Neda&ubreve;ra planto</ref>:
      <ekz>
        unujara <klr>(vivas nur unu jaron)</klr>,
        somera <klr>(&gcirc;ermas printempe kaj fruktas somere
        a&ubreve; a&ubreve;tune)</klr>,
        vintra <klr>(&gcirc;ermas a&ubreve;tune kaj fruktas
        sekvajare printempe)</klr>,
        dujara <klr>(&gcirc;ermas en unu jaro kaj floras kaj
        fruktas en sekva jaro)</klr>, plurjara <tld/>o.
      </ekz>
    </dif>
    <trd lng="de">Kraut</trd>
  </snc>
  <trd lng="cs">tr&aacute;va</trd>
  <trd lng="en">grass</trd>
  <trd lng="fr">herbe</trd>
  <trd lng="nl">gras</trd>
  <trd lng="pl">trawa</trd>
  <trd lng="ru">&c_t;&c_r;&c_a;&c_v;&c_a;</trd>
  <trd lng="sv">gr&auml;s</trd>
</drv>

<drv mrk="herb.0acxo">
  <kap>
    <tld/>a&ccirc;o
  </kap>
  <snc>
    <dif>
      <tld lit="H"/>o, malutila por la kulturado.
    </dif>
  </snc>
  <trd lng="de">Unkraut</trd>
  <trd lng="fr">mauvaise <ind>herbe</ind></trd>
  <trd lng="nl">onkruid</trd>
  <trdgrp lng="pl">
    <trd>zielsko</trd>,
    <trd>chwast</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_r;&c_n;&c_ja;&c_k;</trd>
</drv>

<drv mrk="herb.0aro0ejo">
  <kap>
    <tld/>aro, <tld/>ejo
  </kap>
  <snc>
    <dif>
      Loko, kie kreskas sova&gcirc;aj a&ubreve; kulturitaj
      pa&scirc;t<tld/>oj.
    </dif>
  </snc>
  <trd lng="de">Wiese</trd>
  <trd lng="fr">prairie</trd>
  <trd lng="nl">weide</trd>
  <trd lng="pl">&lstroke;&aogonek;ka</trd>
  <trd lng="ru">&c_l;&c_u;&c_g;</trd>
</drv>

<drv mrk="herb.hund0o">
  <kap>
    hund<tld/>o
  </kap>
  <snc>
    <ref tip="dif" cel="agropi.0o">agropiro</ref>
  </snc>
  <trd lng="fr">chiendent</trd>
  <trd lng="nl">kweek</trd>
  <trd lng="pl">perz</trd>
</drv>

<drv mrk="herb.veruk0o">
  <kap>
    veruk<tld/>o
  </kap>
  <snc>
    <ref tip="dif" cel="kelido.0o">kelidonio</ref>
  </snc>
  <trd lng="fr">ch&eacute;lidoine</trd>
  <trd lng="nl">stinkende <ind>gouwe</ind></trd>
  <trdgrp lng="pl">
    <trd>glistnik</trd>,
    <trd>jask&oacute;&lstroke;cze <ind>ziele</ind></trd>
  </trdgrp>
</drv>
</art>
