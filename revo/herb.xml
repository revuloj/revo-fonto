<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>herb</rad>/o
    </kap>

    <drv mrk="herb.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="herb.0o.KOMUNE">
        <dif>
          Nealta natura vegetalaro, kiel tiu kovranta la grundon en
          <refgrp tip="malprt">
            <ref cel="step.0o">stepo</ref>,
            <ref cel="pasxt.0ejo">pa&scirc;tejo</ref>
          </refgrp> a&ubreve; <ref tip="malprt" cel="gazon.0o">gazono</ref>,
          konsistanta el unujaraj a&ubreve; plurjaraj radikhavaj vegetaloj
          kun maldika trunko neligna:
          <ekz>
            vojon batitan <tld/>o ne kovras
            <fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            depost tiu tempo la remparo fortiki&gcirc;is kaj balda&ubreve;
            kovri&gcirc;is per abunda <tld/>o
            <fnt><bib>Fab2</bib>, <lok>El fenestro en Vartou</lok></fnt>;
          </ekz><ekz>
            per <tld/>o oni vin nutros kiel bovojn
            <fnt><bib>MT</bib>, <lok>&Dan; 4:25</lok></fnt>;
          </ekz><ekz>
            kiel <tld/>o, matene &gcirc;i floras kaj &gcirc;ermas, vespere
            &gcirc;i dehaki&gcirc;as kaj seki&gcirc;as
            <fnt><bib>MT</bib>, <lok>&Psa; 90:5</lok></fnt>;
          </ekz><ekz>
            venis akridoj kaj skaraboj sennombraj kaj forman&gcirc;is
            la tutan <tld/>on en ilia lando
            <fnt><bib>MT</bib>, <lok>&Psa; 105:34</lok></fnt>.
          </ekz>
        </dif>
        <rim>
          La&ubreve; PIV2 tiusenca <tld/>o konsistas el
          <ref tip="vid" cel="poac.0oj">poacoj</ref>; kvankam iuj el
          ili (ekz-e festuko, elimo ktp) efektive estas tipaj herbospecoj,
          tamen reale temas pri multe pli vasta nocio.  La koncepto
          ne estas bazita sur botanika klasifiko, &gcirc;i estas ekologia kaj
          mastrumada: <tld/>o<sncref ref="herb.0o.KOMUNE"/> estas
          <ref tip="super" cel="form.vivo0o">vivoformo</ref> (anka&ubreve;
          <ref tip="sub" cel="foli.tri0o">trifolio</ref> kaj
          <ref tip="sub" cel="luzern.0o">luzerno</ref> estas <tld/>oj, sen esti
          poacoj); kaj en la mastrumado <tld/>o estas nocio, kontrasta al
          <ref tip="ant" cel="gren.0o.planto">greno</ref>: seka <tld/>o
          estas <ref tip="vid" cel="fojn.0o">fojno</ref>, seka greno
          estas pajlo.  Tritiko estas poaco, sed komunlingve &gcirc;i ne
          estas <tld/>o.
          
          Jen a&ubreve;toritataj ekzemploj pri nepoaca <tld/>o:
          <ekz>
            &ccirc;ar kiel floro de <tld/>o li forpasos
            <fnt><bib>NT</bib>, <lok>&Jak; 1:10</lok></fnt>;
          </ekz><ekz>
            la verda <tld/>o kun la malgrandaj ru&gcirc;aj kaj blankaj
            floroj estis la subpieda tapi&scirc;o
            <fnt><bib>Fab1</bib>, <lok>Vojkamarado</lok></fnt>.
          </ekz>
          <aut>Sergio Pokrovskij</aut>
        </rim>
        <trdgrp lng="de">
          <trd>Kraut</trd>,
          <trd><ofc>*</ofc>Gras</trd>
        </trdgrp>
      </snc>

      <snc mrk="herb.0o.ero">
        <uzo tip="stl">RAR</uzo>
        <ref tip="dif" cel="herb.0ero"><tld/>ero</ref>
        <ekz>
          homojn kaj brutojn kaj &ccirc;iun <tld/>on de la kampo
          batdifektis la hajlo kaj &ccirc;iujn arbojn de la kampo
          &gcirc;i rompis
          <fnt><bib>MT</bib>, <lok>Eliro 9:25?</lok></fnt>.
        </ekz>
      </snc>

      <snc mrk="herb.0o.BOT">
        <uzo tip="fak">BOT</uzo>
        <ref tip="dif" cel="plant.nedauxra0o">Neda&ubreve;ra planto</ref>
        <ekz>
          unujara <klr>(vivas nur unu jaron)</klr>,
          somera <klr>(&gcirc;ermas printempe kaj fruktas somere
            a&ubreve; a&ubreve;tune)</klr>,
          vintra <klr>(&gcirc;ermas a&ubreve;tune kaj fruktas
            sekvajare printempe)</klr>,
          dujara <klr>(&gcirc;ermas en unu jaro kaj floras kaj
            fruktas en sekva jaro)</klr>, plurjara <tld/>o.
        </ekz>
        <trd lng="de">Kraut</trd>
      </snc>
      <trd lng="be">&c_t;&c_r;&c_a;&c_v;&c_a;</trd>  
      <trd lng="br">geotenn</trd>
      <trd lng="cs">tr&aacute;va</trd>
      <trd lng="en"><ofc>*</ofc>grass</trd>
      <trd lng="es">hierba</trd>
      <trd lng="fr"><ofc>*</ofc>herbe</trd>
      <trd lng="hu">f&udblac;</trd>
      <trd lng="nl">gras</trd>
      <trd lng="pl"><ofc>*</ofc>trawa</trd>
      <trd lng="pt">erva</trd>
      <trd lng="ru"><ofc>*</ofc>&c_t;&c_r;&c_a;&c_v;&c_a;</trd>
      <trd lng="sv">gr&auml;s</trd>
      <trd lng="tp">kasi</trd>
    </drv>

    <drv mrk="herb.0ero">
      <kap><tld/>ero</kap>
      <snc>
        <dif>
          Unuopa individua vegetalo el tiaj, kiuj konsistigas
          <ref tip="malprt" cel="herb.0o.KOMUNE"><tld/>on<sncref/></ref>:
          <ekz>
            Floro falis ankora&ubreve;! seki&gcirc;is <tld/>ero kaj mortis,
            Velkis pro vintra la frosto &gcirc;ia verdanta folio!
	    <fnt>
	      <aut>V. Langlet</aut>:
              <vrk>Al la memoro de Jozef Wasniewski</vrk>.
	      <bib>FK</bib>
	    </fnt>;
          </ekz><ekz>
            marta mateno, ||
            mateno suna, ||
            lastaj nebuloj, unuaj <tld/>eroj
            <fnt>
              <aut>Clelia Guglielminetti</aut>: <vrk>Marto 1945</vrk>.
              <lok>Nica Literatura Revuo, 4:2, p. 53</lok>
            </fnt>;
          </ekz><ekz>
            la re&gcirc;idinoj rigardis unu la alian, rigardis sur la teron
            kaj inter la <tld/>eroj ili ekvidis grandajn nuksojn
            <fnt>
              <vrk><url
                  ref="http://kastoraklubo.w.interia.pl/tri/index.html"
                  >Tri re&gcirc;idinoj</url></vrk>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="ru">&c_t;&c_r;&c_a;&c_v;&c_i;&c_n;&c_k;&c_a;</trd>
    </drv>

    <drv mrk="herb.0acxo">
      <kap><tld/>a&ccirc;o</kap>
      <snc mrk="herb.0acxo.AGR">
        <uzo tip="fak">AGR</uzo>
        <dif>
          <tld lit="H"/>o, malutila por la kulturado:
          <ekz>
            inter <tld/>a&ccirc;oj kaj urtikoj &gcirc;i ku&scirc;is tie en angulo
            <fnt><bib>Fab2</bib>, <lok>abio</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_p;&c_u;&c_s;&c_t;&c_a;&c_z;&c_je;&c_l;&c_mol;&c_l;&c_je;</trd> 
      <trd lng="br">louzaouenn fall</trd>
      <trd lng="de">Unkraut</trd>
      <trd lng="es"><ind>hierba</ind> mala</trd>
      <trd lng="fr">mauvaise herbe</trd>
      <trdgrp lng="hu">
        <trd>gaz</trd>,
        <trd>dudva</trd>
      </trdgrp>
      <trd lng="nl">onkruid</trd>
      <trdgrp lng="pl">
        <trd>zielsko</trd>,
        <trd>chwast</trd>
      </trdgrp>
      <trd lng="pt">erva daninha</trd>
      <trd lng="ru">&c_s;&c_o;&c_r;&c_n;&c_ja;&c_k;</trd>
    </drv>

    <drv mrk="herb.0ejo">
      <kap><tld/>ejo</kap>
      <snc mrk="herb.0ejo.AGR">
        <uzo tip="fak">AGR</uzo>
        <dif>
          Loko, kie kreskas sova&gcirc;a a&ubreve; kulturata <tld/>o:
          <ekz>
            florori&ccirc;a, sufi&ccirc;e granda <tld/>ejo
            <fnt><bib>Marta</bib></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_l;&c_u;&c_g;</trd>   
      <trdgrp lng="br">        
        <trd>foeneg</trd>,
        <trd>geoteg</trd>,
        <trd>prad</trd>
      </trdgrp>
      <trd lng="de">Wiese</trd>
      <trdgrp lng="fr">
        <trd>prairie</trd>,
        <trd>pr&eacute;</trd>
      </trdgrp>
      <trd lng="hu">r&eacute;t</trd>
      <trd lng="nl">weide</trd>
      <trd lng="pl">&lstroke;&aogonek;ka</trd>
      <trd lng="pt">prado</trd>
      <trd lng="ru">&c_l;&c_u;&c_g;</trd>
    </drv>

    <drv mrk="herb.birdo0o">
      <kap>birdo<tld/>o</kap>
      <snc mrk="herb.birdo0o.BOT">
        <uzo tip="stl">KOMUNE</uzo>
        <ref tip="dif" cel="stelari.meza0o.BOT">meza stelario</ref>
        <ekz>
          la sturno fajfis, kiel oni lin lernigis, kaj la avino pendigis
          birdo<tld/>on super la ka&gcirc;o
          <fnt><bib>Fab3</bib>, <lok>&ccirc;e la plej ekstrema maro</lok></fnt>.
        </ekz>
      </snc>                        
      <trdgrp lng="br">
        <trd>brignen-logod</trd>,
        <trd>glered</trd>
      </trdgrp>
      <trd lng="fr">mouron blanc</trd>
      <trd lng="hu">ty&uacute;kh&uacute;r</trd>
      <trd lng="la">Stellaria media</trd>
      <trd lng="pt">erva-de-passarinho</trd>
    </drv>

    <drv mrk="herb.hund0o">
      <kap>hund<tld/>o</kap>
      <snc mrk="herb.hund0o.BOT">
        <uzo tip="stl">KOMUNE</uzo>
        <ref tip="dif" cel="agropi.0o">agropiro</ref>
      </snc>                      
      <trd lng="be">&c_p;&c_y;&c_r;&c_n;&c_ib;&c_k;</trd>
      <trd lng="br">treuzgeotenn</trd>
      <trd lng="es">grama</trd>
      <trd lng="fr">chiendent</trd>
      <trd lng="hu">b&uacute;zaf&udblac;</trd>
      <trd lng="nl">kweek</trd>
      <trd lng="pl">perz</trd>
      <trd lng="pt">grama</trd>
      <trd lng="ru">&c_p;&c_y;&c_r;&c_je;&c_j;</trd>
    </drv>

    <drv mrk="herb.veruk0o">
      <kap>veruk<tld/>o</kap>
      <snc mrk="herb.veruk0o.BOT">
        <uzo tip="stl">KOMUNE</uzo>
        <ref tip="dif" cel="kelido.0o">kelidonio</ref>
      </snc>
      <trd lng="be">&c_p;&c_a;&c_d;&c_t;&c_y;&c_n;&c_mol;&c_n;&c_ib;&c_k;</trd> 
      <trd lng="br">louzaouenn-ar-plac'h-koant</trd>
      <trd lng="fr">ch&eacute;lidoine</trd>
      <trd lng="hu">fecskef&udblac;</trd>
      <trd lng="nl">stinkende <ind>gouwe</ind></trd>
      <trdgrp lng="pl">
        <trd>glistnik</trd>,
        <trd>jask&oacute;&lstroke;cze <ind>ziele</ind></trd>
      </trdgrp>
      <trdgrp lng="pt">
        <trd>celid&ocirc;nia</trd>, 
        <trd>ceruda</trd>, 
        <trd>erva-das-verrugas</trd>, 
        <trd>erva-andorinha</trd> 
      </trdgrp>
      <trd lng="ru">&c_ch;&c_i;&c_s;&c_t;&c_o;&c_t;&c_je;&c_l;</trd>
    </drv>
  </art>
