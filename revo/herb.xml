    <trd>&c_t;&c_r;&c_a;&c_v;&c_i;&c_c;&c_ja;</trd>
  </trdgrp>
  <trd lng="zh">&#x839B; <pr>t&#xED;ng</pr></trd>
</drv>

<drv mrk="herb.0acxo">
  <kap><tld/>a&ccirc;o,
  <var><kap>trud<tld/>o</kap></var>,
  <var><kap>mis<tld/>o</kap></var></kap>
  <snc mrk="herb.0acxo.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      <tld lit="H"/>o malutila por kultivo:
      <ekz>
        inter <tld/>a&ccirc;oj kaj urtikoj &gcirc;i ku&scirc;is tie en
        angulo
        <fnt><bib>Fab2</bib> <lok>Abio</lok></fnt>;
      </ekz>
      <ekz>
        planto, elfosita inter <tld/>a&ccirc;oj kaj arbetoj
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2008/009355.php">Nordiaj rozoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kampa cirsio estas konsiderata kiel &gcirc;ena trud<tld/>o, &ccirc;ar en kultivejoj &gcirc;i
        disvasti&gcirc;as krom semoj de radikpecoj
        <fnt>
          <vrk><url
          ref="https://nordanaturo.wordpress.com/2010/08/28/planto-de-la-semajno-35-cirsio-kampa/">Planto
          de la semajno 35: cirsio kampa</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        nun sur la kampoj kreskas nur mis<tld/>oj,
        kaj homoj devas &ccirc;irka&ubreve;veturadi, migri
        al Moskvo kaj Leningrado por trovi laboron
        <fnt>
          <aut>K. Kniivil&auml;</aut>
          <vrk>Homoj de Putin</vrk>
          <lok>2016</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_s;&c_t;&c_a;&c_z;&c_je;&c_l;&c_mol;&c_l;&c_je;</trd>
  <trd lng="bg">&c_p;&c_l;&c_je;&c_v;&c_je;&c_l;</trd>
  <trd lng="br">louzaouenn fall</trd>
  <trd lng="de">Unkraut</trd>
  <trd lng="en">weed</trd>
  <trd lng="es"><ind>hierba</ind> mala</trd>
  <trd lng="fr">mauvaise herbe</trd>
  <trdgrp lng="hu">
    <trd>gaz</trd>,
    <trd>dudva</trd>
  </trdgrp>
  <trd lng="id">gulma</trd>
  <trdgrp lng="it">
    <trd>erbaccia</trd>,
    <trd>pianta<klr> infestante</klr></trd>,
    <trd>malerba</trd>
  </trdgrp>
  <trd lng="ja">&#x96D1;&#33609;  <pr>&#12374;&#12387;&#x305D;&#12358;</pr></trd>
  <trd lng="nl">onkruid</trd>
  <trdgrp lng="pl">
    <trd>zielsko</trd>,
    <trd>chwast</trd>
  </trdgrp>
  <trd lng="pt">erva daninha</trd>
  <trd lng="ru">&c_s;&c_o;&c_r;&c_n;&c_ja;&c_k;</trd>
  <trd lng="uk">&c_b;&c_u;&c_r;&#8217;&c_ja;&c_n;</trd>
  <trdgrp lng="zh">
    <trd>&#x91CE;&#33609; <pr>y&#x11B;c&#x1CE;o</pr></trd>,
    <trd>&#26434;&#33609; <pr>z&#xE1;c&#x1CE;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="herb.0ejo">
  <kap><ofc>*</ofc><tld/>ejo <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="&AdE;/fundamento/universala_vortaro.html#av-5263">herb'ej'</url></lok></fnt></kap>
  <snc mrk="herb.0ejo.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Loko, kie kreskas sova&gcirc;a a&ubreve; kulturata <tld/>o:
      <ekz>
        la <tld/>ejoj kovri&gcirc;as per brutaroj kaj la kampoj vesti&gcirc;as per greno
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 65:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        fajro ekstermis la <tld/>ejojn de la stepo
        <fnt>
          <bib>MT</bib>
          <lok>Joel 1:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fojno sur la verdaj <tld/>ejoj staris en fojnamasoj, kaj la cikonio
        promenadis sur siaj longaj ru&gcirc;aj piedoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        mar&ccirc;ejo, &ccirc;irka&ubreve;ita &ccirc;iuflanke de malsekaj <tld/>ejoj
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;afoj kaj bovoj pa&scirc;ti&gcirc;adis sur la <tld/>ejo
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;irka&ubreve;e verdis bela <tld/>ejo, kovrita de primoloj, sova&gcirc;aj timianoj kaj miloj
        da aliaj floroj, meze murmuris klara rivereto
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_u;&c_g;</trd>
