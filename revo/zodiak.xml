<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>zodiak</rad>/o
</kap>
<drv mrk="zodiak.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      &Ccirc;iela zono, kies mezon okupas la ekliptiko, 
      kaj kiu enhavas &ccirc;iujn poziciojn de la planedoj, 
      kaj krome la 12 stelarojn <klr>(signoj de <tld/>o)</klr>, 
      kiujn &scirc;ajnas trairi la suno dum sia jara rondiro.  
    </dif>
    <refgrp tip="prt">
      <ref cel="bov.0o">Bovo</ref>,
      <ref cel="fisx.0oj">Fi&scirc;oj</ref>,
      <ref cel="gxemel.0oj">&Gcirc;emeloj</ref>,
      <ref cel="kankr.0o">Kankro</ref>,
      <ref cel="kaprik.0o">Kaprikorno</ref>,
      <ref cel="leon.0o">Leono</ref>,
      <ref cel="pesil.0o">Pesilo</ref>,
      <ref cel="paf.0isto">Pafisto</ref>,
      <ref cel="skorpi.0o">Skorpio</ref>,
      <ref cel="sxaf.0o">&Scirc;afo</ref>,
      <ref cel="versx.0isto">Ver&scirc;isto</ref>,
      <ref cel="virg.0ulino">Virgulino</ref>
    </refgrp>
  </snc>
  <trd lng="de">Tierkreis</trd>
</drv>
</art>
