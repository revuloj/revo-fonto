  <trd lng="fr">n&oelig;ud gordien</trd>
  <trd lng="pl">w&eogonek;ze&lstroke; gordyjski</trd>
  <trd lng="ro">nodul gordian</trd>
  <trd lng="uk">&c_g;&c_o;&c_r;&c_d;&c_ib;&c_ji;&c_v; &c_v;&c_u;&c_z;&c_o;&c_l;</trd>
</drv>

<drv mrk="nod.limfo0o">
  <kap>limfo<tld/>o</kap>
  <snc mrk="nod.limfo0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      &Ccirc;e la mamuloj kaj birdoj, &ccirc;iu el la molaj
      <ref cel="tuber.0o.sxvelajxo">tuber</ref>etoj
      grupe situantaj en certaj lokoj la&ubreve;longe de la
      <ref tip="malprt" cel="limf.0aangio">limfa angio</ref> kaj
      filtrantaj la <ref cel="limf.0o.ZOO">limfo</ref>n:
      <ekz>
        diki&gcirc;o de unu flanko de la kolo estas ka&ubreve;zita de
        &scirc;veli&gcirc;o de limfo<tld/>oj a&ubreve; dentodoloro
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url ref="http://es.chinabroadcast.cn/1/2005/03/18/1@22350.htm">
          Fruaj simptomoj de malsanoj</url></vrk>,
          <lok>2005-03-18</lok>
        </fnt>;
      </ekz>
      <ekz>
        karakterizaj simptomoj estas pligrandi&gcirc;intaj doloraj limfo<tld/>oj, precipe en
        la malanta&ubreve;a kola kaj okcipita regionoj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2012/010767.php">Elimino atingeblas</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        krom en la cerbo, tiuj prionoj povas aperi en limfo<tld/>oj, lieno, okulbulboj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2006/008861.php">Nova etapo en biologio kaj medicino</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_m;&c_f;&c_a;&c_v;&c_u;&c_z;&c_je;&c_l;</trd>
  <trd lng="ca">gangli limf&agrave;tic</trd>
  <trd lng="de">Lymphknoten</trd>
  <trd lng="es">ganglio linf&aacute;tico</trd>
  <trd lng="fr">ganglion lymphatique</trd>
  <trd lng="hu">nyirokcsom&oacute;</trd>
  <trd lng="it">linfonodo</trd>
  <trd lng="ja">&#x30EA;&#x30F3;&#x30D1;&#x7BC0;  <pr>&#x30EA;&#x30F3;&#x30D1;&#x305B;&#x3064;</pr></trd>
  <trd lng="la">nodus lymphaticus</trd>
  <trd lng="nl">lymfeknoop</trd>
  <trd lng="pl">gruczo&lstroke; limfatyczny</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_i;&c_m;&c_f;&c_o;&c_u;&c_z;&c_je;&c_l;</trd>,
    <trd>&c_l;&c_i;&c_m;&c_f;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; &c_u;&c_z;&c_je;&c_l;</trd>
  </trdgrp>
  <trd lng="uk">&c_l;&c_ib;&c_m;&c_f;&c_o;&c_v;&c_u;&c_z;&c_o;&c_l;</trd>
  <trdgrp lng="zh">
    <trd>&#x6DCB;&#x5DF4;&#x7D50; <pr>l&#xED;nb&#x101;ji&#xE9;</pr></trd>,
    <trd>&#x6DCB;&#x5DF4;&#x7ED3; <pr>l&#xED;nb&#x101;ji&#xE9;</pr></trd>,
