<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>nod</rad>/o
</kap>
<drv mrk="nod.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Stre&ccirc;a kunliganta interplekta&jcirc;o; ligotubero.
    </dif>
    <refgrp tip="sin">
      <ref cel="masx.0o">ma&scirc;o</ref>.
    </refgrp>
    <trd lng="nl">knoop</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ligilo:
      <ekz>
        <tld/>oj de amikeco.
      </ekz>
    </dif>
    <trd lng="nl">knoop</trd>
  </snc>
  <snc>
    <dif>
      Kruci&gcirc;ejo:
      <ekz>
        <tld/>o de valoj kaj montoj<fnt>B</fnt>;
      </ekz>
      <ekz>
        Sirio estas <tld/>o de gravaj komunikvojoj.
      </ekz>
    </dif>
    <trd lng="nl">knooppunt</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Centro, plej grava punkto:
      <ekz>
        oni fine malkovris la <tld/>on de la intrigo.
      </ekz>
    </dif>
    <trd lng="nl">knoop</trd>
  </snc>
  <snc>
    <uzo tip="fak">MAR</uzo>
    <dif>
      Distancomezuro egala je 852 m.
    </dif>
    <trd lng="nl">knoop</trd>
  </snc>
  <trd lng="de">Knoten</trd>
  <trd lng="en">knot</trd>
  <trd lng="ru">&c_u;&c_z;&c_je;&c_l;</trd>
</drv>
</art>
