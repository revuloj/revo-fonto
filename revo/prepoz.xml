<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>prepozici</rad>/o
</kap>
<drv mrk="prepoz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="prepoz.0o.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      Senfleksia vorto, kiu anta&ubreve;iras substantivan grupon
      (kun kerno plejofte substantiva, pronoma a&ubreve; infinitiva)
      por indiki ties rolon rilate al verbo, substantivo a&ubreve;
      adjektivo.
    </dif>
    <refgrp tip="vid">
      <ref cel="al.0">al</ref>,
      <ref cel="anstat.0">anstata&ubreve;</ref>,
      <ref cel="antaux.0">anta&ubreve;</ref>,
      <ref cel="antaux.mal0">malanta&ubreve;</ref>,
      <ref cel="cxe.0">&ccirc;e </ref>,
      <ref cel="cxirka.0">&ccirc;irka&ubreve;</ref>,
      <ref cel="de.0">de</ref>,
      <ref cel="da.0">da</ref>,
      <ref cel="dum.0">dum</ref>,
      <ref cel="ekster.0">ekster</ref>,
      <ref cel="el.0">el</ref>,
      <ref cel="en.0">en</ref>,
      <ref cel="gxis.0">&gcirc;is</ref>,
      <ref cel="je.0">je</ref>,
      <ref cel="kontraux.0">kontra&ubreve;</ref>,
      <ref cel="krom.0">krom</ref>,
      <ref cel="kun.0">kun</ref>,
      <ref cel="laux.0">la&ubreve;</ref>,
      <ref cel="malgra.0">malgra&ubreve;</ref>,
      <ref cel="per.0">per</ref>,
      <ref cel="po.0">po</ref>,
      <ref cel="por.0">por</ref>,
      <ref cel="post.0">post</ref>,
      <ref cel="preter.0">preter</ref>,
      <ref cel="pri.0">pri</ref>,
      <ref cel="pro.0">pro</ref>,
      <ref cel="sen.0">sen</ref>,
      <ref cel="sub.0">sub</ref>,
      <ref cel="super.0">super</ref>,
      <ref cel="sur.0">sur</ref>,
      <ref cel="tra.0">tra</ref>,
      <ref cel="trans.0">trans</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_n;&c_a;&c_z;&c_o;&c_w;&c_n;&c_ib;&c_k;</trd>
  <trd lng="cs">p&rcaron;edlo&zcaron;ka</trd>
  <trdgrp lng="de">
    <trd>Verh&auml;ltniswort</trd>,
    <trd>Pr&auml;position</trd>
  </trdgrp>
  <trd lng="en">preposition</trd>
  <trd lng="fr">pr&eacute;position</trd>
  <trdgrp lng="hu">
    <trd>el&ouml;lj&aacute;r&oacute;sz&oacute;</trd>,
    <trd>viszonysz&oacute;</trd>,
    <trd>prepoz&iacute;ci&oacute;</trd>
  </trdgrp>
  <trd lng="nl">voorzetsel</trd>
  <trd lng="pt">preposi&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_l;&c_o;&c_g;</trd>
</drv>
</art>
