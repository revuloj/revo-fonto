<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>prepozici</rad>/o
</kap>
<drv mrk="prepoz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Senfleksia vorto, kiu ligas substantivon a&ubreve; infinitivon al alia
      subst. a&ubreve; infinitivo per difinita rilato de dependeco, ekz.
      <ekz>
        al, de, preter, por.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_n;&c_a;&c_z;&c_o;&c_w;&c_n;&c_ib;&c_k;</trd>
  <trd lng="cs">p&rcaron;edlo&zcaron;ka</trd>
  <trdgrp lng="de">
    <trd>Verh&auml;ltniswort</trd>,
    <trd>Pr&auml;position</trd>
  </trdgrp>
  <trd lng="en">preposition</trd>
  <trd lng="fr">pr&eacute;position</trd>
  <trd lng="nl">voorzetsel</trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_l;&c_o;&c_g;</trd>
</drv>
</art>
