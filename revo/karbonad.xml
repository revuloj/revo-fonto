<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: karbonad.xml,v 1.3 2026/01/17 02:23:10 afido Exp $">
    <kap>
      <rad>karbonad</rad>/o
      <fnt><bib>JED</bib></fnt>
    </kap>

<drv mrk="karbonad.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc mrk="karbonad.0o.KUI">
    <dif>
      Sur karboj kuirita <ref tip="super" cel="viand.0o">viando</ref>:
      <ekz>
        oni surtabligos gria&jcirc;on kaj <tld/>on
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tisto de porkoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne plu konvenas starigi gigantan inkvizician brul&scirc;tiparon kaj flamigi flagojn kaj
        standardojn
        <klr>[&#8230;]</klr> same kiel oni iam <tld/>igis herezulojn
        <fnt>
          <bib>Monato</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Karbonade</trd>
    <trd lng="en">grilled cutlet</trd>
    <trd lng="fr">carbonade</trd>
    <trd lng="pl">karbonada  <klr>(mi&eogonek;so)</klr></trd>
  </snc>
<trd lng="be">&c_k;&c_a;&c_r;&c_b;&c_a;&c_n;&c_a;&c_d;</trd>
</drv>
</art>

