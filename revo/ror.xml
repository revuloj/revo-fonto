<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ror</rad>/i <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="ror.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ror.0i.ZOO">
    <dif>
      <ref tip="lst" cel="blek.0i" lst="voko:blekoj">Bleki</ref>
      forte, elgor&gcirc;e, kiel leono a&ubreve; tia granda besto:
      <ekz>
        la monstro <tld/>is denove
        <fnt>
          <aut>R. Bradbury, trad. M. Giorgini</aut>,
          <vrk><url
          ref="http://yksi.org/tekst/ald/bradbury_foghorn-eo.html">
          La nebulsireno</url></vrk>,
          <lok>[2009]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>feuler</trd>,
    <trd>rugir</trd>
  </trdgrp>
  <trd lng="sk">reva≈• <klr>(tiger| lev)</klr></trd>
</drv>

<drv mrk="ror.0o">
  <kap><tld/>o</kap>
  <snc mrk="ror.0o.ZOO">
    <dif>
      Ra&ubreve;ka bleko de granda, sova&gcirc;a besto.
    </dif>
  </snc>
  <trd lng="fr">rugissement</trd>
  <trd lng="sk">rev <klr>(tiger| lev)</klr></trd>
</drv>
</art>

