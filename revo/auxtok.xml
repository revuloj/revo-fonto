<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>a&ubreve;tokrat</rad>/o
</kap>
<drv mrk="auxtok.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Absoluta regnestro.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_m;&c_a;&c_d;&c_z;&c_je;&c_r;&c_zh;&c_a;&c_c;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_a;&c_w;&c_l;&c_a;&c_d;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Alleinherrscher</trd>,
     <trd>Autokrat</trd>
  </trdgrp>
  <trd lng="en">autocrat</trd>
  <trd lng="es">aut&oacute;crata</trd>
  <trd lng="fr">autocrate<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>egyeduralkod&oacute;</trd>,
    <trd>autokrata</trd>
  </trdgrp>
  <trd lng="nl">autocraat</trd>
  <trd lng="ru">&c_s;&c_a;&c_m;&c_o;&c_d;&c_je;&c_r;&c_zh;&c_je;&c_c;</trd>
  <trdgrp lng="sk">
    <trd>samovl&aacute;dca</trd>,
    <trd>autokrat</trd>
  </trdgrp>
</drv>

<drv mrk="auxtok.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      Sistemo de absoluta regado.
    </dif>
  </snc>
  <ref tip="sin" cel="auxtok1.0o">a&ubreve;tokratio</ref>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_m;&c_a;&c_d;&c_z;&c_ja;&c_r;&c_zh;&c_a;&c_w;&c_je;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_a;&c_w;&c_l;&c_a;&c_d;&c_z;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Alleinherrschaft</trd>,
     <trd>Autokratie</trd>
  </trdgrp>
  <trd lng="en">autocracy</trd>
  <trd lng="es">autocracia</trd>
  <trd lng="fr">autocratie</trd>
  <trdgrp lng="hu">
    <trd>&ouml;nk&eacute;nyuralom</trd>,
    <trd>autokr&aacute;cia</trd>
  </trdgrp>
  <trd lng="nl">autocratie</trd>
  <trd lng="ru">&c_s;&c_a;&c_m;&c_o;&c_d;&c_je;&c_r;&c_zh;&c_a;&c_v;&c_i;&c_je;</trd>
  <trd lng="sk">autokracia</trd>
</drv>
</art>
