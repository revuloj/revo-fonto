<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: kazablank.xml,v 1.2 2022/05/01 08:09:24 afido Exp $">
    <kap>
      <rad>Kazablank</rad>/o
    </kap>

<drv mrk="kazablank.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Plej granda <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      de <ref tip="malprt" cel="marok.M0o">Maroko</ref>
      kun haveno &ccirc;e Atlantiko, sude de la &ccirc;efurbo <ref cel="rabat1.0o">Rabato</ref>:
      <ekz>
        la Moskeo Hasano la 2-a trovi&gcirc;as en la urbo <tld/>o <klr>[&#8230;]</klr> kaj estas la
        plej alta templo
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Moskeo_Hasano_la_2-a">Moskeo Hasano la 2-a</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        preska&ubreve; 80&#8239;% de la dek mil afrikaj migruloj en Maroko estas grupigitaj en
        Rabato, kaj iom malpli, en <tld/>o
        <fnt>
          <vrk><url ref="&Mondediplo;article1228.html">La longa &ccirc;asado de eksterle&gcirc;aj migrantoj en Maroko</url></vrk>
          <lok>Le Monde diplomatique' en Esperanto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ar">&a_A;&a_l;&a_d;&a_A;&a_r; &a_A;&a_l;&a_b;&a_y;&a_D;&a_A;&a_hamza;</trd>
    <trd lng="de">Casablanca</trd>
    <trd lng="en">Casablanca</trd>
    <trd lng="fr">Casablanca</trd>
  </snc>
</drv>

  </art>

