<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sinkron.xml,v 1.7 2023/09/03 18:39:30 revo Exp $">
<kap>
  <rad>sinkron</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="sinkron.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="sinkron.0a.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="dif" cel="temp.sam0a.KOMUNE">Samtempa</ref>,
      <ref tip="dif" cel="takt.sam0a.KOMUNE">samtakta</ref>;
      konsideranta aferojn en difinita momento, sen tempa evoluo;
      kontraste al <ref tip="ant" cel="diakron.0a.SCI">diakrona</ref>:
      <ekz>
        etaj galaksioj, kiuj movi&gcirc;as interne de la Andromeda,
        <klr>...</klr> rotacias la&ubreve; <tld/>a rapideco
        <fnt>
          <aut>P. S. Viana</aut>,
          <vrk><url ref="http://retradio.webnode.at/news/dek-kvin-jara-sciencisto/">
          Dek-kvin-jara sciencisto</url></vrk>,
          <lok>esperanta retradio, 2013-01-08</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_y;&c_n;&c_h;&c_r;&c_o;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_d;&c_n;&c_a;&c_ch;&c_a;&c_s;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_a;&c_d;&c_n;&c_a;&c_ch;&c_a;&c_s;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="en">synchronic</trd>
  <trd lng="fr">synchronique</trd>
  <trdgrp lng="ja">
    <trd>&#x540C;&#x6642;&#x306B;&#x8D77;&#x304D;&#x308B;  [<ind>&#x3069;&#x3046;&#x3058;&#x306B;&#x304A;&#x304D;&#x308B;</ind>]</trd>,
    <trd>&#x540C;&#x671F;&#x306E;  [<ind>&#x3069;&#x3046;&#x304D;&#x306E;</ind>]</trd>,
    <trd>&#x540C;&#x671F;&#x3057;&#x305F;  [<ind>&#x3069;&#x3046;&#x304D;&#x3057;&#x305F;</ind>]</trd>,
    <trd>&#x5171;&#x6642;&#x7684;&#x306A;  [<ind>&#x3068;&#x3082;&#x3058;&#x3066;&#x304D;&#x306A;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="pl">
   <trd>jednoczesny</trd>,
   <trd>synchroniczny</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>synchronick&yacute;</trd>,
    <trd>s&uacute;&ccaron;asn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&#x43D;&c_h;&c_r;&#x43E;&#x43D;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43E;&c_d;&#x43D;&#x43E;&c_ch;&c_a;&c_s;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>

