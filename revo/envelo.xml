<art mrk="$Id: envelo.xml,v 1.13 2022/12/15 08:34:11 revo Exp $">
<kap>
  <rad>envelop</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="envelo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="volv.en0ajxo.de_kurboj">Envolva&jcirc;o</ref>:
      <ekz>
        &leftquot;sonkoloro&rightquot; tamen nur figurasence estas nomebla parametro, &ccirc;ar &gcirc;i ne konsistas
        el variablo kun diskreta valoro, sed estas tempe &scirc;an&gcirc;ebla paso de suprotona
        spektro kaj amplitudo resp. <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=287002">Parametro (muziko)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_g;&c_o;&c_r;&c_t;&c_k;&c_a; <klr tip="amb">(&c_s;&c_ja;&c_m;&#8217;&c_ib; &c_k;&c_r;&c_y;&c_v;&c_y;&c_h;)</klr></trd>
  <trdgrp lng="cs">
    <trd>obal</trd>,
    <trd>obalov&aacute; k&rcaron;ivka</trd>,
    <trd>ob&aacute;lka</trd>,
    <trd>pl&aacute;&scaron;&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Einh&uuml;llende</trd>,
     <trd>Umh&uuml;llungskurve</trd>,
     <trd>Envolvente</trd>
  </trdgrp>
  <trd lng="en">envelope</trd>
  <trd lng="fr">enveloppe<klr tip="ind"> (d'une famille de
  courbes)</klr></trd>
  <trd lng="hu">burkol&oacute;g&ouml;rbe</trd>
  <trdgrp lng="nl">
    <trd>enveloppe</trd>,
    <trd>omslag</trd>
  </trdgrp>
  <trd lng="pl">obwiednia<klr tip="ind"> (rodziny krzywych)</klr></trd>
  <trdgrp lng="pt">
    <trd>envelope</trd>,
    <trd>involt&oacute;rio</trd>,
    <trd>inv&oacute;lucro</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_g;&c_i;&c_b;&c_a;&c_ju;&c_shch;&c_a;&c_ja;<klr tip="ind"> (&c_s;&c_je;&c_m;&c_je;&c_s;&c_t;&c_v;&c_a;
  &c_k;&c_r;&c_i;&c_v;&c_y;&c_h;)</klr></trd>
  <trdgrp lng="sk">
    <trd>obal</trd>,
    <trd>ob&aacute;lka</trd>,
    <trd>pl&aacute;&scaron;&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_je;&c_v;&#x43E;&#x43B;&#x44C;&c_v;&c_je;&#x43D;&c_t;&c_a;</trd>
</drv>
</art>

