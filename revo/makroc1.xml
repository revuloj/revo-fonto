<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: makroc1.xml,v 1.13 2020/05/03 14:10:20 revo Exp $">
<kap>
  <rad>makrocefaled</rad>/oj,
  <var><kap><rad>ka&ccirc;aloted</rad>/oj</kap></var>
</kap>
<drv mrk="makroc1.0oj">
  <kap>
    <tld/>oj,
    <var><kap>ka&ccirc;alotedoj</kap></var>
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt" cel="cetac.0oj">cetacoj</ref>
      (<trd lng="la">Physeteridae</trd>):
      <ekz>
        al la familio <tld/>oj apartenas tri vivantaj specioj:
        makrocefalo (<nom>Physeter macrocephalus</nom>),
        kaj du kogioj (<nom>Kogia simus</nom>, <nom>Kogia
        breviceps</nom>).
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_sh;&c_a;&c_l;&c_jo;&c_t;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_k;&c_a;&c_sh;&c_a;&c_l;&c_o;&c_t;&c_i; &c_d;&c_zh;&c_u;&c_d;&c_zh;&c_je;&c_t;&c_a;</trd>
  <trd lng="ca">fiset&egrave;rids</trd>
  <trd lng="de">Pottwale<klr tip="ind">(Familie)</klr></trd>
  <trd lng="en">sperm whale</trd>
  <trd lng="es">cachalote</trd>
  <trd lng="fr">cachalot</trd>
  <trd lng="hu">fiz&eacute;ter-f&eacute;l&eacute;k</trd>  
  <trd lng="it">capodoglio</trd>
  <trd lng="nl">potvissen</trd>
  <trd lng="pl">kaszalotowate</trd> 
  <trd lng="pt">fiseter&iacute;deos</trd>
  <trd lng="ro">physeteroidea</trd>
  <trd lng="ru">&c_k;&c_a;&c_sh;&c_a;&c_l;&c_o;&c_t;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>

