<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&jcirc;erz</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="jxerz.0o">
  <kap><tld/>o</kap>
  <snc mrk="jxerz.0o.TEKS_sxtofo">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Ma&scirc;ine trikita <ref tip="super" cel="sxtof.0o.TEKS">
      &scirc;tofo</ref> iom
      elasta, nomita la&ubreve; la devena manikinsulo &Jcirc;erzo.
    </dif>
    <trd lng="de">Jersey<klr tip="amb"> (Stoff)</klr></trd>
    <trd lng="pl">jersey<klr> (materia&lstroke;)</klr></trd>
    <trd lng="it">jersey<klr> (tessuto)</klr></trd>
  </snc>
  <snc mrk="jxerz.0o.TEKS_vesto">
    <dif>
      Vesto farita el <tld/>o&scirc;tofo, tipe iom strikta tia
      <ref tip="vid" cel="pulover.0o.TEKS">pulovero</ref>:
      <ekz>
        en la saketon &scirc;i metis <klr>...</klr> blankan <tld/>on, 
        kalsoneton kaj duon&scirc;trumpojn
        <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 15a, p. 91a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">jersey<klr tip="amb"> (tissu, v&ecirc;tement)</klr></trd>
  <trd lng="pl">jersey<klr> (sweter)</klr></trd>
  <trdgrp lng="pt">
    <trd>j&eacute;rsei</trd>
  </trdgrp>
  <trd lng="it">golf<klr tip="amb"> (maglione)</klr></trd>
</drv>
</art>

