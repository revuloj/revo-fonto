<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: produk1.xml,v 1.14 2019/09/22 18:10:17 revo Exp $">
<kap>
  <rad>produktiv</rad>/a
  <fnt><bib>EE</bib></fnt>
</kap>
<drv mrk="produk1.0a">
  <kap><tld/>a</kap>
  <snc mrk="produk1.0a.multa">
    <dif>
      <ref tip="dif" cel="produk.0ema">Produktema</ref>:
      <ekz>
        tio &ccirc;i englutas ja ne<tld/>e grandegan multon da
        laboroj
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        pro &gcirc;ia alta <tld/>eco oni &gcirc;in nomas &leftquot;superrizo&#8221;
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="https://www.monato.be/2003/008031.html"
              >Superrizo vivtenos pli da &ccirc;inoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la grundo estas mal<tld/>a pro sen&ccirc;esa ekspluatado
        <fnt>
          <bib>Monato</bib>
          <aut>Sagbadjelou Kokou</aut>
          <vrk><url ref="https://www.monato.be/2005/008658.php"
              >Pro pluvo plu</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="produk1.0a.pova">
    <dif>
      Produktanta; produktopova:
      <ekz>
        impostoj el <tld/>a laboro
        <fnt>
          <bib>Monato</bib>
          <aut>Bertil Englund</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007035.html"
              >Elefanto en porcelanbutiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;inaj entreprenoj <esc>plejparte</esc> havas malaltan labor<tld/>econ
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="https://www.monato.be/2004/008325.html"
              >Kvin defioj por &ccirc;inaj urboj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la ekologia piedsigno estas la <tld/>a areo, kiun homo bezonas por produkti la
        varojn, kiujn li konsumas
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="https://www.monato.be/2010/010051.php"
              >Monda akvo fluas en malbonaj direktoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_d;&c_u;&c_k;&c_c;&c_y;&c_j;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_d;&c_u;&c_k;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>produktivn&iacute;</trd>,
    <trd>tvo&rcaron;iv&yacute;</trd>,
    <trd>v&yacute;konn&yacute;</trd>
  </trdgrp>
  <trd lng="de">produktiv</trd>
  <trd lng="fr">productif</trd>
  <trdgrp lng="hu">
    <trd>termel&eacute;keny</trd>,
    <trd>produkt&iacute;v</trd>
  </trdgrp>
  <trd lng="nl">productief</trd>
  <trd lng="pt">produtivo</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_d;&c_u;&c_k;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>plodn&yacute;</trd>,
    <trd>produkt&iacute;vny</trd>
  </trdgrp>
</drv>
</art>
