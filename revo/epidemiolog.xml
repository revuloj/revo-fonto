<vortaro>
<art mrk="$Id: epidemiolog.xml,v 1.10 2023/06/10 12:39:11 revo Exp $">
<kap>
  <rad>epidemiolog</rad>/o
</kap>
<drv mrk="epidemiolog.0o">
  <kap><tld/>o</kap>
  <snc mrk="epidemiolog.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="dif" cel="epidemiologi.0isto.MED">Epidemiologiisto</ref>:
      <ekz>
        du kuracistoj, unu <tld/>o kaj unu eksperto pri infektaj
        malsanoj laboras &ccirc;e medicina centro en la urbo Landstulo
        <fnt>
          <aut>&Gcirc;angalo</aut>,
          <vrk><url
          ref="http://gxangalo.com/modules/news/article.php?storyid=791">
          Mistera pne&ubreve;monito...</url></vrk>,
          <lok>2003-08-05</lok>
        </fnt>;
      </ekz>
      <ekz>
        plej forte timas gripon virusologoj kaj <tld/>oj, &ccirc;ar ili tre bone scias, ke la
        gripviruso povas ka&ubreve;zi vastegajn epidemiojn
        <fnt>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2003/008099.html"
              >Birda gripo, &ccirc;u dan&gcirc;era por la homo?</url></vrk>
              <bib>Monato</bib>
          <lok>2003/06, p. 14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_p;&c_ib;&c_d;&c_e;&c_m;&c_ib;&c_jo;&c_l;&c_ja;&c_g;</trd>
  <trd lng="cs">epidemiolog</trd>
  <trd lng="de">Epidemiologe</trd>
  <trd lng="en">epidemiologist</trd>
  <trd lng="fr">&eacute;pid&eacute;miologiste</trd>
  <trd lng="nl">epidemioloog</trd>
  <trd lng="pl">epidemiolog</trd>
  <trd lng="sk">epidemiol&oacute;g</trd>
</drv>
</art>

