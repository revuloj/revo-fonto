<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
	<kap><ofc>*</ofc><rad>acer</rad>/o</kap>

<drv mrk="acer.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="acerac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Acer</trd>), kun manforme entran&ccirc;itaj folioj
      kaj flugilhavaj fruktoj.
    </dif>
  </snc>
  <bld lok="../bld/acer.jpg">
    folioj kaj fruktoj de <ind>monta <tld/>o</ind>
    <trd lng="la">Acer pseudoplatanus</trd>
  </bld>
  <trdgrp lng="be">
    <trd>&c_k;&c_l;&c_jo;&c_n;</trd>, 
    <trd>&c_ja;&c_v;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="cs">javor</trd>
  <trd lng="de">Ahorn</trd>
  <trd lng="fr">&eacute;rable</trd>
  <trdgrp lng="hu">
    <trd>juharfa</trd>,
    <trd>j&aacute;vorfa</trd>
  </trdgrp>
  <trd lng="it">acero</trd>
  <trd lng="en">maple</trd>
  <trdgrp lng="nl">
    <trd>ahorn</trd>,
    <trd>esdoorn</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_l;&c_jo;&c_n;</trd>
  <trd lng="sv">l&ouml;nn</trd>
</drv>

</art>
