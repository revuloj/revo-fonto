<art mrk="$Id: acer.xml,v 1.43 2023/10/12 16:57:42 revo Exp $">
    <kap><ofc>*</ofc><rad>acer</rad>/o</kap>

<drv mrk="acer.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio 
      <ref tip="malprt" cel="acerac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Acer</trd>), kun manforme entran&ccirc;itaj folioj
      kaj flugilhavaj fruktoj:
      <ekz>
        mi komencis a&ccirc;eti belajn arbojn kaj plantis ilin en la herbaro:
        ekzemple <tld/>ojn kun bela &scirc;elo, &leftquot;plorantan&rightquot; fagon, pirarbon
        kaj pomujojn
        <fnt>
          <aut>M&aacute;ire NicAoidh</aut>
          <vrk><url ref="&Monato;/2000/006878.php">&Gcirc;ardenumi</url></vrk>
          <bib>Monato</bib>
          <lok>2000/04, p. 20</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/acer.jpg">
    folioj kaj fruktoj de <ind>monta <tld/>o</ind>
    <trd lng="la">Acer pseudoplatanus</trd>
    <trdgrp lng="br">
      <trd>sikamorenn</trd>,
      <trd>skavenn-wrac'h-wenn</trd>
    </trdgrp>
    <trd lng="fa">&a_A;&a_f;&a_r;&a_A;&f_y; &a_sh;&a_b;&a_h;&zwnj;&f_ch;&a_n;&a_A;&a_r;&f_y;</trd>
    <trdgrp lng="fr">
      <trd>faux platane</trd>,
      <trd>sycomore</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>tyskl&ouml;nn</trd>,
      <trd>sykomorl&ouml;nn</trd>
    </trdgrp>
  </bld>
  <trdgrp lng="be">
    <trd>&c_k;&c_l;&c_jo;&c_n;</trd>, 
    <trd>&c_ja;&c_v;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="bg">&c_ja;&c_v;&c_o;&c_r;</trd>
  <trdgrp lng="br">
    <trd>gwezenn skav-gwrac'h</trd>,
    <trd>skavenn-wrac'h</trd>
  </trdgrp>
  <trd lng="ca">aur&oacute;</trd>
  <trd lng="cs">javor</trd>
  <trdgrp lng="da">
    <trd>&aelig;r</trd>,
    <trd>ahorn</trd>
  </trdgrp>
  <trd lng="de">Ahorn</trd>
  <trd lng="en">maple</trd>
  <trd lng="es">arce</trd>
  <trd lng="fa">&a_A;&a_f;&a_r;&a_A;</trd>
  <trd lng="fr">&eacute;rable</trd>
  <trd lng="he">&#x5E4;&#x5BC;&#x5B0;&#x5DC;&#x5B8;&#x5D3;&#x5B8;&#x5D4;</trd>
  <trdgrp lng="hu">
    <trd>juharfa</trd>,
    <trd>j&aacute;vorfa</trd>
  </trdgrp>
  <trd lng="hr">javor</trd>
  <trd lng="id">mapel</trd>
  <trd lng="it">acero</trd>
  <trd lng="ja">&#x30AB;&#x30A8;&#x30C7; <pr>&#x304B;&#12360;&#12391;</pr></trd>
  <trdgrp lng="nl">
    <trd>ahorn</trd>,
    <trd>esdoorn</trd>
  </trdgrp>
  <trd lng="pl">klon</trd>
  <trd lng="ru">&c_k;&c_l;&c_jo;&c_n;</trd>
    <trd lng="sk">javor</trd>
  <trd lng="sv">l&ouml;nn</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43B;&c_je;&#x43D;</trd>,
    <trd>&#x44F;&c_v;&c_ib;&c_r;</trd>
  </trdgrp>
</drv>

