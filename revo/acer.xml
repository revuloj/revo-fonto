<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc><rad>acer</rad>/o</kap>
<drv mrk="acer.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="acer.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Acer</trd>), kun manforme entran&ccirc;itaj folioj
      kaj flugilhavaj fruktoj.  
    </dif>
  </snc>
  <trd lng="cs">javor</trd>
  <trd lng="de">Ahorn</trd>
  <trd lng="it">acero</trd>
</drv>
<drv mrk="acer.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj 
      (<trd lng="la">Aceraceae</trd>), arboj kaj arbedoj, kies
      fruktoj havas du flugilojn.
    </dif>
  </snc>
  <trd lng="de">Ahorngew&auml;chse</trd>
</drv>
</art>
