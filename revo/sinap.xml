<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>sinap</rad>/o
</kap>
<drv mrk="sinap.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="brasik1.0oj">brasikacoj</ref>
      (<trd lng="la">Sinapis</trd>), herbo kun semoj uzataj por prepari
      <ref tip="vid" cel="mustar.0o">mustardon</ref> kaj fari kataplasmojn:
      <ekz>
        se vi havas fidon, kiel semero de <tld/>o, vi diros al &ccirc;i tiu
        monto: Transloki&gcirc;u tien de &ccirc;i tie, kaj &gcirc;i transloki&gcirc;os
        <fnt><bib>NT</bib>, <lok>&Mat; 17:20</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/sinap.jpg">
    <ind>kampa <tld/>o</ind>
    <trdgrp lng="fr">
      <trd>moutarde noire<klr> (bot.)</klr></trd>,
      <trd>s&eacute;nev&eacute;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>vad <ind>repce</ind></trd>,
      <trd>vet&eacute;si <ind>repce</ind></trd>,
      <trd>mezei <ind>must&aacute;r</ind></trd>,
    </trdgrp>
    <trd lng="la">Sinapis arvensis</trd>
  </bld>
  <trd lng="be">&c_g;&c_a;&c_r;&c_ch;&c_y;&c_c;&c_a; <klr tip="amb">(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trd lng="de">Senf</trd>
  <trd lng="fr">moutarde<klr> (bot.)</klr></trd>
  <trd lng="hu">repce</trd>
  <trd lng="nl">mosterdplant</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_ch;&c_i;&c_c;&c_a;</trd>
</drv>
</art>
