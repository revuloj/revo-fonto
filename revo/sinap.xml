<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sinap.xml,v 1.21 2018/10/07 19:10:18 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>sinap</rad>/o
</kap>
<drv mrk="sinap.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref>
      el familio <ref tip="malprt" cel="brasik1.0oj">brasikacoj</ref>
      (<trd lng="la">Sinapis</trd>), <ref tip="lst" cel="herb.0o.BOT" 
      lst="voko:brasikoparencoj">herbo</ref> de kies
      unu specio <trd lng="la">Sinapis alba</trd> oni uzas la
      malgrandajn blankajn globformajn semojn por prepari
      <ref tip="vid" cel="mustar.0o">mustardon</ref>:
      <ekz>
        se vi havas fidon, kiel semero de <tld/>o, vi diros al &ccirc;i
        tiu monto: transloki&gcirc;u tien de &ccirc;i tie, kaj &gcirc;i
        transloki&gcirc;os
        <fnt><bib>NT</bib>, <lok>&Mat; 17:20</lok></fnt>,
      </ekz>
      <ekz>
        &ccirc;iu el ili fari&gcirc;as potenca, kiel la Biblia grajno de <tld/>o, elkreskas kiel arbo
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
       lok="&WCU;/thumb/9/92/Sinapis_arvensis_ENBLA02.jpg/512px-Sinapis_arvensis_ENBLA02.jpg"
       prm="CC BY-SA 4.0"
       ><ind>kampa <tld/>o</ind> (Sinapis arvensis), Valo Belluna, Italujo2006
         <fnt>
           <aut>Enrico Blasutto, propra verko</aut>
           <lok><url ref="&WCW;/File:Sinapis_arvensis_ENBLA02.jpg"
               >&WCW;/File:Sinapis_arvensis_ENBLA02.jpg</url></lok>
         </fnt>
      <trd lng="ca">ravenissa borda</trd>
      <trd lng="fa">&a_kh;&a_r;&a_d;&a_l; &a_b;&f_y;&a_A;&a_b;&a_A;&a_n;&f_y;</trd>
      <trdgrp lng="fr">
        <trd>moutarde noire<klr> (bot.)</klr></trd>,
        <trd>s&eacute;nev&eacute;</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>vad <ind>repce</ind></trd>,
        <trd>vet&eacute;si <ind>repce</ind></trd>,
        <trd>mezei <ind>must&aacute;r</ind></trd>,
      </trdgrp>
      <trd lng="la">Sinapis arvensis</trd>
      <trd lng="pl">gorczyca polna</trd>
    </bld>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_ch;&c_y;&c_c;&c_a; <klr
  tip="amb">(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trd lng="ca">mostassa<klr> (planta)</klr></trd>
  <trd lng="de">Senf</trd>
  <trd lng="en">mustard<klr> plant</klr></trd>
  <trd lng="fa">&a_kh;&a_r;&a_d;&a_l;<klr tip="ind"> (&f_g;&f_y;&a_A;&a_h;)</klr></trd>
  <trd lng="fr">moutarde<klr> (bot.)</klr></trd>
  <trd lng="hu">repce</trd>
  <trd lng="nl">mosterdplant</trd>
  <trd lng="pl">gorczyca</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_ch;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">hor&ccaron;ica <klr>(rastlina)</klr></trd>
  <trd lng="cs">ho&rcaron;&ccaron;ice</trd>
</drv>
</art>
