<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>sinap</rad>/o
</kap>
<drv mrk="sinap.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="brasik.0acoj">brasikacoj</ref>
      (<trd lng="la">Sinapis</trd>), herbo uzata por prepari
      <ref tip="vid" cel="mustar.0o">mustardon</ref> kaj fari kataplasmojn.
    </dif>
  </snc>
  <bld lok="../bld/sinap.jpg">
    <ind>kampa <tld/>o</ind>
    <trd lng="la">Sinapis arvensis</trd>
  </bld>
  <trd lng="de">Senf</trd>
</drv>
</art>
