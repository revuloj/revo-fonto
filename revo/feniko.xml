<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: feniko.xml,v 1.30 2024/02/25 01:26:50 revo Exp $">
<kap>
  <rad>fenikopter</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="feniko.0o">
  <kap><tld/>o</kap>
  <snc mrk="feniko.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda longakrura <ref tip="lst" lst="voko:birdoj" cel="bird.0o.ZOO">birdo</ref>
      kun rozkoloraj kaj ru&gcirc;aj plumoj, ofte staranta sur nur unu piedo,
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref>
      el <ref tip="malprt" cel="feniko1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Phoenicopterus</trd>):
      <ekz>
        somere envenis la regionon iu alilanda grupo da <tld/>oj, belaj rozkoloraj
        dupiedaj fieruloj
        <fnt>
          <bib>Monato</bib>
          <aut>Angela Tellier</aut>
          <vrk><url ref="&Monato;/2004/008306.html">La&ubreve;proverbaj fabeloj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/feniko.jpg">
    <tld/>oj
  </bld>
  <trd lng="be">&c_f;&c_l;&c_ja;&c_m;&c_ib;&c_n;&c_g;&c_a; <klr tip="amb">(&c_p;&c_t;&c_u;&c_sh;&c_k;&c_a;)</klr></trd>
  <trd lng="bg">&c_f;&c_l;&c_a;&c_m;&c_i;&c_n;&c_g;&c_o;</trd>
  <trd lng="cs">plame&ncaron;&aacute;k</trd>
  <trd lng="de">Flamingo</trd>
  <trd lng="en"><klr>(greater) </klr>flamingo</trd>
  <trd lng="fa">&a_f;&a_l;&a_A;&a_m;&f_y;&a_n;&f_g;&a_w;</trd>
  <trd lng="fr">flamant</trd>
  <trd lng="hu">flaming&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x30D5;&#x30E9;&#x30DF;&#x30F3;&#x30B4;</trd>,
    <trd>&#x30D9;&#x30CB;&#x30C5;&#x30EB;</trd>
  </trdgrp>
  <trd lng="nl">flamingo</trd>
  <trdgrp lng="pl">
    <trd>czerwonak</trd>,
    <trd>flaming<klr> (r&oacute;&zdot;owy)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>flamingo</trd>,
    <trd>flamengo</trd>,
    <trd>fenic&oacute;ptero</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_l;&c_a;&c_m;&c_i;&c_n;&c_g;&c_o;</trd>
  <trd lng="sv">flamingo</trd>
  <trd lng="sk">plameniak</trd>
  <trd lng="uk">&c_f;&#x43B;&c_a;&#x43C;&c_ib;&#x43D;&c_gu;&#x43E;</trd>
  <trd lng="zh">&#x706B;&#x70C8;&#x9CE5; <pr>hu&#x1D2;li&#xE8;ni&#x1CE;o</pr></trd>
</drv>

<drv mrk="feniko.0oformaj">
  <kap><tld/>oformaj</kap>
  <snc mrk="feniko.0oformaj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref> el la klaso <ref
      tip="malprt" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Phoenicopteriformes</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_l;&c_ja;&c_m;&c_ib;&c_n;&c_g;&c_a; <klr tip="amb">(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="cs">plame&ncaron;&aacute;ci</trd>
  <trd lng="de">Flamingov&ouml;gel</trd>
  <trd lng="fr">ph&oelig;nicopt&eacute;riformes</trd>
  <trd lng="hu">flaming&oacute;alak&uacute;ak</trd>
  <trd lng="ru">&c_f;&c_l;&c_a;&c_m;&c_i;&c_n;&c_g;&c_o;<klr tip="amb">
  (&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trd lng="sk">plameniaci</trd>
</drv>
</art>
