<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>fenikopter</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="feniko.0o">
  <kap><tld/>o</kap>
  <snc mrk="feniko.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda longakrura birdo kun rozkoloraj kaj ru&gcirc;aj plumoj,
      ofte staranta sur nur unu piedo,
      genro el <ref tip="super" cel="feniko1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Phoenicopterus</trd>).
    </dif>
  </snc>
  <bld lok="../bld/feniko.jpg">
    <tld/>oj
  </bld>
  <trd lng="be">&c_f;&c_l;&c_ja;&c_m;&c_ib;&c_n;&c_g;&c_a;</trd>
  <trd lng="cs">plame&ncaron;&aacute;k</trd>
  <trd lng="de">Flamingo</trd>
  <trd lng="en">flamingo</trd>
  <trd lng="fr">flamant</trd>
  <trd lng="hu">flaming&oacute;</trd>
  <trd lng="nl">flamingo</trd>
  <trd lng="pl">czerwonak</trd>
  <trdgrp lng="pt">
    <trd>flamingo</trd>,
    <trd>flamengo</trd>,
    <trd>fenic&oacute;ptero</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_l;&c_a;&c_m;&c_i;&c_n;&c_g;&c_o;</trd>
</drv>

<drv mrk="feniko.0oformaj">
  <kap><tld/>oformaj</kap>
  <snc mrk="feniko.0oformaj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Phoenicopteriformes</trd>).
    </dif>
  </snc>
  <trd lng="fr">ph&oelig;nicopt&eacute;riformes</trd>
  <trd lng="hu">flaming&oacute;alak&uacute;ak</trd>
  <trd lng="ru">&c_f;&c_l;&c_a;&c_m;&c_i;&c_n;&c_g;&c_o;<klr tip="amb"> (&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
</drv>
</art>
