<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cxial.xml,v 1.16 2021/03/06 09:01:35 revo Exp $">
  <kap>
    <ofc>*</ofc> <ofc>7</ofc> <rad>&ccirc;ial</rad>
    <fnt><bib>PV</bib></fnt>
  </kap>
  <drv mrk="cxial.0">
    <kap><ofc>*</ofc><tld/></kap>
    <snc>
      <uzo tip="klr">(adv)</uzo>
      <dif>
        Pro &ccirc;ia motivo, &ccirc;ia ka&ubreve;zo: <ekz>tio estas
        <tld/> konsilinda.
      </ekz>
      </dif>
    </snc>
    <trd lng="be">&c_p;&c_a; &c_w;&c_s;&c_ja;&c_l;&c_ja;&c_k;&c_a;&c_j;
    <ind>&c_p;&c_r;&c_y;&c_ch;&c_y;&c_n;&c_je;</ind></trd>
    <trdgrp lng="de">
       <trd>aus jedem beliebigen Grund</trd>
    </trdgrp>
    <trd lng="fr">pour toute <ind>raison</ind></trd>
    <trd lng="he">&#x5DE;&#x5DB;&#x5DC; &#x5E1;&#x5D9;&#x5D1;&#x5D4; &#x5E9;&#x5D4;&#x5D9;&#x5D0;</trd>
  <trdgrp lng="hu">
       <trd>minden&eacute;rt <klr tip="ind"> (ok)</klr></trd>,
       <trd>minden <ind>okn&aacute;l</ind> fogva</trd>,
       <trd>minden <ind>miatt</ind></trd>
    </trdgrp>
 <trdgrp lng="pt">
   <trd>por qualquer motivo</trd>,
   <trd>por todos os motivos</trd>
 </trdgrp>
    <trdgrp lng="pl">
      <trd>z ka&zdot;dego <ind>powodu</ind></trd>,
      <trd>w ka&zdot;dym <ind>przypadku</ind></trd>,
      <trd>w ka&zdot;dym <ind>wypadku</ind></trd>,
      <trd>tak czy owak</trd>,
      <trd>tak czy siak</trd>
    </trdgrp>
    <trd lng="ro">pentru tot felul de motive</trd>
    <trd lng="ru">&c_p;&c_o; &c_v;&c_s;&c_ja;&c_k;&c_o;&c_j;
    &c_p;&c_r;&c_i;&c_ch;&c_i;&c_n;&c_je;</trd>
    <trd lng="sv">av alla <ind>sk&auml;l</ind></trd>
  </drv>
</art>
