<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: driad.xml,v 1.19 2024/06/29 10:30:24 revo Exp $">
<kap>
  <rad>driad</rad>/o
  <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="driad.0o">
  <kap><tld/>o</kap>
  <snc mrk="driad.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      <klr>(en greka mitologio)</klr>
      Arbara <ref tip="super" cel="nimf.0o.MIT">nimfo</ref>:
      <ekz>
        en &gcirc;i lo&gcirc;as vivanta osta&jcirc;o, virino, &gcirc;i estas nomata <tld/>o, &gcirc;i naski&gcirc;as kun la
        arbo kaj mortas kun &gcirc;i
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        unuj nomis min sambuka virineto, aliaj nomis min <tld lit="D"/>o, sed en
        efektiveco mi estas nomata rememoro
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni vidis grupojn da homoj, alivestitaj kiel fa&ubreve;noj a&ubreve; satirusoj, ludantaj
        flutojn, &scirc;almojn, tamburetojn, kaj grupojn da knabinoj, prezentantaj nimfojn,
        <tld/>ojn kaj hamadriadojn
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXXI</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="hamadriad.0o">hamadriado</ref>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_y;&c_ja;&c_d;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>dry&aacute;da</trd>,
    <trd>jezinka</trd>,
    <trd>lesn&iacute; pana</trd>,
    <trd>lesn&iacute; v&iacute;la</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Dryade</trd>,
     <trd>Baumnymphe</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>dr&iacute;ade</trd>,
    <trd>dr&iacute;ada</trd>
  </trdgrp>
  <trd lng="fr">dryade</trd>
  <trd lng="ja">&#12489;&#12522;&#12517;&#12450;&#12473;</trd>
  <trd lng="pl">driada</trd>
  <trdgrp lng="pt">
    <trd>dr&iacute;ade</trd>,
    <trd>dr&iacute;ada</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_r;&c_i;&c_a;&c_d;&c_a;</trd>
  <trd lng="sk">dry&aacute;dka osemlupienkov&aacute;</trd>
  <trd lng="sv">dryad</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_r;&c_ib;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_l;&c_ib;&c_s;&c_o;&c_v;&c_a; &c_n;&c_ib;&c_m;&c_f;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
