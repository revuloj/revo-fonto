<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: driad.xml,v 1.15 2019/01/18 18:10:22 revo Exp $">
<kap>
  <rad>driad</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="driad.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIT</uzo>
  <snc>
    <dif>
      Arbara nimfo:
      <ekz>
        en &gcirc;i lo&gcirc;as vivanta osta&jcirc;o, virino, &gcirc;i estas nomata <tld/>o, &gcirc;i naski&gcirc;as kun la
        arbo kaj mortas kun &gcirc;i
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz> 
      <ekz>
        unuj nomis min sambuka virineto, aliaj nomis min <tld lit="D"/>o, sed en
        efektiveco mi estas nomata rememoro
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_y;&c_ja;&c_d;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>dry&aacute;da</trd>,
    <trd>jezinka</trd>,
    <trd>lesn&iacute; pana</trd>,
    <trd>lesn&iacute; v&iacute;la</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Dryade</trd>,
     <trd>Baumnymphe</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>dr&iacute;ade</trd>,
    <trd>dr&iacute;ada</trd>
  </trdgrp>
  <trd lng="fr">dryade</trd>
  <trd lng="pl">driada</trd>
  <trdgrp lng="pt">
    <trd>dr&iacute;ade</trd>,
    <trd>dr&iacute;ada</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_r;&c_i;&c_a;&c_d;&c_a;</trd>
  <trd lng="sk">dry&aacute;dka osemlupienkov&aacute;</trd>
  <trd lng="sv">dryad</trd>
</drv>
</art>
