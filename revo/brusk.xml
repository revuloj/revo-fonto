<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: brusk.xml,v 1.14 2025/03/28 16:17:59 revo Exp $">
<kap>
  <rad>brusk</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="brusk.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="brusk.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="trakt.mis0i.KOMUNE">Mistrakti</ref>,
      malmilde, sendeteni&gcirc;e agi kontra&ubreve;:
      <ekz>
        tio iel <tld/>is &scirc;ian memregon
        <fnt><aut>S. Larbar</aut>, <vrk>Karuseloj</vrk>,
        <lok>1987</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
      <trd>&#20882;&#29359; <pr>m&#224;of&#224;n</pr></trd>,
      <trd>&#24471;&#32618;&#20154; <pr>d&#233;zu&#236;r&#233;n</pr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_o;&c_r;&c_s;&c_t;&c_k;&c_a; &c_s;&c_t;&c_a;&c_v;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_g;&c_r;&c_u;&c_b;&c_a; &c_a;&c_b;&c_y;&c_h;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>obo&rcaron;it se</trd>,
    <trd>osopit se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>br&uuml;skieren</trd>,
    <trd>vor den <ind>Kopf</ind> sto&szlig;en</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>brusquer</trd>,
    <trd>rudoyer</trd>
  </trdgrp>
  <trd lng="he">&#1500;&#1492;&#1506;&#1500;&#1497;&#1489;</trd>
  <trdgrp lng="pl">
    <trd>by&cacute; szorstkim <klr>dla kogo&sacute;</klr></trd>,
    <trd>by&cacute; niedelikatnym <klr>dla kogo&sacute;</klr></trd>
  </trdgrp>
  <trd lng="sk">osopi&tcaron; sa</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_ib;&c_z;&#1082;&#1086;/&c_g;&c_r;&c_u;&c_b;&#1086; &#1087;&#1086;&c_v;&#1086;&c_d;&c_i;&c_t;&c_i;&c_s;&#1103;</trd>,
    <trd>&c_z;&#1085;&c_u;&c_shch;&c_a;&c_t;&c_i;&c_s;&#1103;</trd>
  </trdgrp>
</drv>

<drv mrk="brusk.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="brusk.0a.KOMUNE">
    <dif>
      <ref tip="dif" cel="abrupt.0a.bruska">Abrupta</ref>:
      <ekz>
        li estis <tld/>a, iom kruda, a&ubreve;toritatema viro
        <fnt>
          <aut>L. Couperus, trad. G. Berveling</aut>,
          <vrk>Pri Maljunuloj, la A&jcirc;oj kiuj Pasas</vrk>,
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        malapero de tradiciaj viv-eroj en landego spertanta moderni&gcirc;on ofte <tld/>an
        <fnt>
       <aut>Istv&aacute;n Ertl</aut>
          <vrk><url ref="&Monato;/2000/006905.php">Emocia &leftquot;du&scirc;o&#8221;, tremiga
          &leftquot;mago&#8221;</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 25</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
       <ref cel="mild.mal0a">malmilda</ref>,
       <ref cel="krud.0a.MOR">kruda<sncref/></ref>,
       <ref cel="drast.0a">drasta</ref>,
       <ref cel="brutal.0a">brutala</ref>
    </refgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>malmilda</ke>, <ke>senpacienca</ke>, <ke>mal&#285;entila</ke>.
  </rim>

 <trdgrp lng="be">
    <trd>&c_zh;&c_o;&c_r;&c_s;&c_t;&c_k;&c_ib;</trd>,
    <trd>&c_g;&c_r;&c_u;&c_b;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_d;&c_a;&c_l;&c_ib;&c_k;&c_a;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bryskn&iacute;</trd>,
    <trd>p&rcaron;&iacute;kr&yacute; <klr>(&rcaron;e&ccaron;)</klr></trd>,
    <trd>ur&aacute;&zcaron;liv&ecaron; &uacute;se&ccaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>br&uuml;sk</trd>,
    <trd>schroff</trd>
  </trdgrp>
  <trd lng="fr">cassant<klr> (abrupt)</klr></trd>
  <trdgrp lng="he">
    <trd>&#1502;&#1506;&#1500;&#1497;&#1489;</trd>,
    <trd>&#1494;&#1506;&#1508;&#1504;&#1497;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>szorstki</trd>,
    <trd>niedelikatny</trd>
    <trd>obcesowy</trd>
  </trdgrp>
  <trd lng="ro">brusc</trd>
  <trdgrp lng="sk">
    <trd>odseknut&yacute; <klr>(o hovore)</klr></trd>,
    <trd>surov&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#1084;&c_a;&#1083;&#1086;&c_v;&c_zh;&c_i;&c_v;.&#160;&c_r;&c_ib;&c_z;&#1082;&c_i;&c_j;</trd>,
    <trd>&c_g;&c_r;&c_u;&c_b;&c_i;&c_j;</trd>,
    <trd>&#1082;&c_r;&c_u;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="brusk.0e">
  <kap>
    <tld/>e
  </kap>
  <snc mrk="brusk.0e.malmilde">
    <dif>
      En <tld/>a maniero:
      <ekz>
        brita soldato <tld/>e ordonanta en la angla lingvo, ke civilulo surgenui&gcirc;u
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2003/008109.html">Taglibro de l' milito ... el la lando de
          &Scirc;ekspiro (3)</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e malfermi&gcirc;antaj kaj refermi&gcirc;antaj pordoj
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="&Monato;/2010/009883.php">Trancporto</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_o;&c_r;&c_s;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_g;&c_r;&c_u;&c_b;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>br&uuml;sk</trd>,
    <trd>barsch</trd>,
    <trd>schroff</trd>
  </trdgrp>
</drv>

</art>

