<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: senes.xml,v 1.8 2023/11/27 09:27:57 revo Exp $">
    <kap>
      <rad>senes</rad>/o <fnt><bib>Far1</bib></fnt>
    </kap>

<drv mrk="senes.0o">
  <kap><tld/>o</kap>
  <snc mrk="senes.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio <ref tip="super" cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Senna</trd>), kaj aparte la Aleksandria <tld/>o
      (<trd lng="la">Senna alexandrina</trd>,
      anta&ubreve;e <trd lng="la">Cassia senna</trd>), kiu
      estas kultivata kaj kies gu&scirc;oj havas laksigan efikon;
      en antikva tempo oni aplikis &gcirc;in kontra&ubreve; lepro, stomakaj kaj okulaj malsanoj:
      <ekz>
        en Malsupra Egipto oni rikoltis la trifolion kaj <tld/>on
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 21a</lok></fnt>.
      </ekz>
    </dif>
    <bld
       lok="&WCU;/thumb/d/dd/Senna_alexandrina_Prague_2017_2.jpg/418px-Senna_alexandrina_Prague_2017_2.jpg"
       prm="CC BY 4.0"
       ><tld/>o (Senna alexandrina), Botanika &Gcirc;ardeno de Karola Universitato en Prago
         <fnt>
           <aut>Karelj, propra verko</aut>
           <lok><url ref="&WCW;/File:Senna_alexandrina_Prague_2017_2.jpg"
               >&WCW;/File:Senna_alexandrina_Prague_2017_2.jpg</url></lok>
         </fnt>
    </bld>
    <trd lng="de">Alexandrinische Senna</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_e;&c_n;&c_a; <klr tip="amb">(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>,
    <trd>&c_k;&c_a;&c_s;&c_ib;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>sena</trd>,
    <trd>senn<klr>(es)</klr>ov&eacute; listy</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Senna</trd>
  </trdgrp>
  <trd lng="fr">s&eacute;n&eacute; d'Alexandrie</trd>
  <trd lng="it">senna<klr> (comun.)</klr></trd>
  <trdgrp lng="pl">
    <trd>senes</trd>,
    <trd>kasja</trd>,
    <trd>str&aogonek;czyniec</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_s;&c_s;&c_i;&c_ja;</trd>,
    <trd>&c_a;&c_l;&c_je;&c_k;&c_s;&c_a;&c_n;&c_d;&c_r;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j; &c_l;&c_i;&c_s;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">sena</trd>
</drv>
  </art>

