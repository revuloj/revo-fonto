<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>senes</rad>/o <fnt><bib>NePiv</bib></fnt>
    </kap>

    <drv mrk="senes.0o">
      <kap><tld/>o</kap>
      <snc mrk="senes.0o.BOT">
        <uzo tip="fak">BOT</uzo>
        <dif>
          Kultivataj vegeta&jcirc;oj, kies gu&scirc;oj havas laksigan efikon
          (<trd lng="la">Cassia senna</trd>):
          <ekz>
            en Malsupra Egipto oni rikoltis la trifolion kaj <tld/>on
            <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 21a</lok></fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="fr">s&eacute;n&eacute; d'Alexandrie</trd>
  <trd lng="it">senna<klr> (comun.)</klr></trd>
  <trdgrp lng="ru">
        <trd>&c_k;&c_a;&c_s;&c_s;&c_i;&c_ja;</trd>,
        <trd>&c_a;&c_l;&c_je;&c_k;&c_s;&c_a;&c_n;&c_d;&c_r;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j;
&c_l;&c_i;&c_s;&c_t;</trd>
      </trdgrp>
  <trd lng="sk">sena</trd>
</drv>
  </art>

