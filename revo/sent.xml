  </trdgrp>
  <trd lng="ru">&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_o; <klr tip="amb">(&c_s;&c_p;&c_o;&c_s;&c_o;&c_b;&c_n;&c_o;&c_s;&c_t;&c_mol;
  &c_v;&c_o;&c_s;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_i;&c_ja;: &c_s;&c_l;&c_u;&c_h;, &c_z;&c_r;&c_je;&c_n;&c_i;&c_je; &c_i; &c_t;.&c_p;.)</klr></trd>
  <trdgrp lng="sk">
    <trd>zmyslov&yacute; org&aacute;n</trd>,
    <trd>&uacute;stroj</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ch;&c_u;&c_t;&c_t;&c_ja;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_ch;&c_u;&c_t;&c_t;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="sent.0umajxo">
  <kap><tld/>uma&jcirc;o</kap>
  <snc mrk="sent.0umajxo.SEN">
    <dif>
      <ref tip="dif" cel="sens.0ajxo.SEN">Sensa&jcirc;o</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="en">sensation</trd>
  <trd lng="fr">sensation</trd>
  <trd lng="hu">&eacute;rzet</trd>
  <trd lng="nl">zintuiglijke <ind>waarneming</ind></trd>
  <trdgrp lng="pl">
    <trd>czucie</trd>,
    <trd>odczucie</trd>,
    <trd>uczucie</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_shch;&c_u;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="sent.antaux0i">
  <kap>anta&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sent.antaux0i.KOMUNE">
    <dif>
      Havi intuan, malklaran ideon pri estonta&jcirc;o:
      <ekz>
        mi anta&ubreve;<tld/>is, ke vi hodia&ubreve; vespere venos
        <fnt><bib>Fab3</bib>, <lok>sub la saliko</lok></fnt>;
      </ekz>
      <ekz>
        post horo li estos en la vila&gcirc;o kaj jam anta&ubreve;sentis
        la plezuron de la ripozo
        <fnt><aut>J. Modest</aut>, <vrk>La ikono</vrk>,
        <bib>Monato</bib>, <lok>2001:12, p.
        23a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vid.antaux0i">anta&ubreve;vidi</ref>,
      <ref cel="progno.0i">prognozi</ref>,
      <ref cel="supoz.0i">supozi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="en">
    <trd>to anticipate</trd>,
    <trd>to have a feeling <klr>(I had a feeling that you would
    come)</klr></trd>
  </trdgrp>
  <trd lng="fr">pressentir</trd>
  <trd lng="hu">meg&eacute;rez</trd>
  <trd lng="nl">een <ind>voorgevoel</ind> hebben</trd>
  <trd lng="pl">przeczuwa&cacute;</trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="es">presentir</trd>
  <trd lng="uk">&c_p;&c_je;&c_r;&c_je;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="sent.devo0o">
  <kap>devo<tld/>o</kap>
  <snc mrk="sent.devo0o.KOMUNE">
    <dif>
      Interna <tld/>o pri devoj, pri morale necesaj sintenoj kaj agoj:
      <ekz>
        la devo<tld/>o &ccirc;iam dum la vivo estis por mi ia feti&ccirc;o
        <fnt>
          <aut>E. R. Burroughs, trad. K. R. C. Sturmer</aut>,
          <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0029/index.php">
          Princino de Marso</url></vrk>,
          <lok>1938</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">sense of obligation</trd>
  <trd lng="fr">sens du devoir</trd>
  <trd lng="nl">plichtsgevoel</trd>
</drv>

<drv mrk="sent.kun0i">
  <kap>kun<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sent.kun0i.KOMUNE">
    <dif>
      <tld lit="S"/>i akorde kun alia persono:
      <ekz>
        mi kun<tld/>as kun vi en viaj afliktoj
        <fnt><bib>I&scirc;tar</bib>,
        <lok>&ccirc;apitro 1a, renkonto</lok></fnt>;
      </ekz>
      <ekz>
        ili verkis libron pri siaj lingvistikaj esploroj, esperante
        ke ni kun<tld/>os ilian fascini&gcirc;on pri lingva
        tempoekskurso
        <fnt><aut>S. Derevjanni&hcirc;</aut>, <vrk>Atrakcio por
        lingvo&scirc;atantoj?</vrk>, <bib>Monato</bib>,
        <lok>2001:11, p. 30a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konsen.0i">konsenti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_a;&c_ch;&c_u;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_g;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>c&iacute;tit s n&ecaron;k&yacute;m</trd>,
    <trd>m&iacute;t soucit</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>to empathize <klr>(usone)</klr></trd>,
    <trd>to empathise <klr>(brite)</klr></trd>, <trd>relate to</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ressentir<klr> (partager les sentiments)</klr></trd>,
    <trd>partager les sentiments</trd>
  </trdgrp>
  <trd lng="hu">egy&uuml;tt <ind>&eacute;rez</ind></trd>
  <trd lng="pl">wsp&oacute;&lstroke;czu&cacute;</trd>
  <trd lng="ru">&c_s;&c_o;&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">ma&tcaron; s&uacute;cit</trd>
  <trd lng="uk">&c_s;&c_p;&c_ib;&c_v;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="sent.kulpo0o">
  <kap>kulpo<tld/>o</kap>
  <snc mrk="sent.kulpo0o.KOMUNE">
    <dif>
      Interna dolora <tld/>o pri propraj malbonaj agoj:
      <ekz>
        pro la kulpo<tld/>o de sia konscienco &scirc;i timis, ke 
        oni spionas &ccirc;iun &scirc;ian iron
        <fnt>
          <aut>E. R. Burroughs, trad. K. R. C. Sturmer</aut>,
          <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0029/index.php">
          Princino de Marso</url></vrk>,
          <lok>1938</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">pocit viny</trd>
  <trdgrp lng="en">
    <trd>guilty conscience</trd>,
    <trd>compunction</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>mauvaise conscience</trd>,
    <trd>sentiment de culpabilit&eacute;</trd>
  </trdgrp>
  <trd lng="nl">schuldgevoel</trd>
  <trd lng="no">skyldf&oslash;lelse</trd>
  <trd lng="sk">pocit viny</trd>
</drv>

<drv mrk="sent.kun0o">
  <kap>kun<tld/>o</kap>
  <snc mrk="sent.kun0o.KOMUNE">
    <dif>
      <tld lit="S"/>ado akorda kun tiu de alia persono:
      <ekz>
        ili man&gcirc;is kun li en lia domo, kaj esprimis al li sian
        kun<tld/>on, kaj konsolis lin
        <fnt><bib>MT</bib>, <lok>&Ijo; 42:11</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="simpat.0o">simpatio</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_a;&c_ch;&c_u;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_g;&c_a;&c_d;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&t_sa;&t_nyata;&t_gigu;&t_nga;&t_tsheg;&t_ra;&t_jata;&t_drengbu;&t_tsheg;</trd>
  <trd lng="cs">soucit</trd>
  <trd lng="de">Mitgef&uuml;hl</trd>
  <trdgrp lng="en">
    <trd>sympathy</trd>,
    <trd>understanding</trd>, <trd>empathy</trd>
  </trdgrp>
  <trd lng="es">consentimiento</trd>
  <trdgrp lng="fr">
    <trd>empathie</trd>,
    <trd>sympathie<klr> (empathie)</klr></trd>
  </trdgrp>
  <trd lng="hu">egy&uuml;tt&eacute;rz&eacute;s</trd>
  <trd lng="nl">medeleven</trd>
  <trd lng="pl">wsp&oacute;&lstroke;czucie</trd>
  <trd lng="ru">&c_s;&c_o;&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  <trd lng="sk">s&uacute;cit</trd>
  <trd lng="uk">&c_p;&c_o;&c_ch;&c_u;&c_t;&c_t;&c_ja;</trd>
</drv>

<drv mrk="sent.kun0emo">
  <kap>kun<tld/>emo</kap>
  <snc mrk="sent.kun0emo.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      Kapablo diveni kaj proprigi sentojn de aliaj homoj:
      <ekz>
        kompatemo kaj kun<tld/>emo estas pli humanaj ol senanima 
        <klr>[&#8230;]</klr> ignorado
        <fnt><aut>U. Matthias</aut>, <vrk><url ref="http://www.u-matthias.de/verko/fajron.htm">Fajron sentas 
        mi interne</url></vrk>, <lok>Pro Esperanto, 1990</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="intuic.0o">intuicio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_m;&c_p;&c_a;&c_t;&c_y;&c_ja;</trd>
  <trd lng="en">empathy</trd>
  <trd lng="fr">empathie</trd>
  <trd lng="nl">empathie</trd>
  <trd lng="no">empati</trd>
  <trdgrp lng="pl">
    <trd>empatia</trd>,
    <trd>wsp&oacute;&lstroke;odczuwanie<klr> (nieformalnie)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="sent.lingvo0o">
  <kap>lingvo<tld/>o</kap>
  <snc mrk="sent.lingvo0o.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      Intuo de parolanto pri la &gcirc;usteco, konveneco de
      parolmaniero:
      <ekz>
        &ccirc;ar &ccirc;iuj menciitaj formoj estas bona Esperanto, oni
        uzu ilin la&ubreve; sia lingvo<tld/>o
        <fnt>
        <aut>D. Besseghini</aut>,
        <vrk>Re- Nur eta korekto</vrk>,
        <lok>soc.culture.esperanto, 1993-05-07</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Sprachgef&uuml;hl</trd>
  <trd lng="en">language sense</trd>
  <trd lng="fr">sentiment linguistique</trd>
  <trd lng="hu">nyelv&eacute;rz&eacute;k</trd>
  <trd lng="nl">taalgevoel</trd>
  <trd lng="no">spr&aring;kf&oslash;lelse</trd>
  <trdgrp lng="pl">
    <trd>wyczucie j&eogonek;zyka</trd>,
    <trd>zdolno&sacute;ci j&eogonek;zykowe</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_o; &c_ja;&c_z;&c_y;&c_k;&c_a;</trd>,
    <trd>&c_ja;&c_z;&c_y;&c_k;&c_o;&c_v;&c_a;&c_ja; &c_i;&c_n;&c_t;&c_u;&c_i;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="sent.ne0eble">
  <kap>ne<tld/>eble</kap>
  <snc mrk="sent.ne0eble.KOMUNE">
    <dif>
      Tiom malforte, diskrete a&ubreve; grade, ke oni apena&ubreve;
      <tld/>as:
      <ekz>
        a&ubreve;gusto forpasis ne<tld/>eble
        <fnt><aut>J. Modest</aut>, <vrk>En la ombro de la
        silento</vrk>, <bib>Monato</bib>, <lok>1997:3, p.
        14a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_a;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_a;</trd>
  <trd lng="en">imperceptibly</trd>
  <trdgrp lng="fr">
    <trd>insensiblement</trd>
  </trdgrp>
  <trd lng="hu">nem <ind>&eacute;rz&eacute;kelhet&odblac;en</ind></trd>
  <trdgrp lng="nl">
    <trd>onvoelbaar</trd>,
    <trd>onmerkbaar</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>niewyczuwalnie</trd>,
    <trd>nieodczuwalnie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_o;&c_shch;&c_u;&c_t;&c_i;&c_m;&c_o;</trd>,
    <trd>&c_n;&c_je;&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;</trd>
  </trdgrp>
  <trd lng="es">imperceptible</trd>
</drv>

<drv mrk="sent.sam0eco">
  <kap>sam<tld/>eco</kap>
  <snc mrk="sent.sam0eco.KOMUNE">
    <dif>
      Stato de personoj same <tld/>antaj.
    </dif>
    <ref tip="vid" cel="akord.0o.samopinieco">akordo</ref>
  </snc>
  <trdgrp lng="en">
    <trd>rapport</trd>,
    <trd>affinity</trd>
  </trdgrp>
</drv>

<drv mrk="sent.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="sent.sen0a.KOMUNE">
    <dif>
      Ne havanta <tld/>on,
      <ref tip="vid" cel="indife.0a">indiferenta</ref>,
      <ref tip="vid" cel="morn.0a">morna</ref>:
    </dif>
    <subsnc mrk="sent.sen0a.PSI">
      <dif>
        Ne havanta <tld/>on emocian a&ubreve; animan:
      <ekz>
        &ccirc;u la multaj zorgoj jam faris &scirc;in sen<tld/>a kaj pri doloro
        kaj 
        pri &gcirc;ojo?
        <fnt><aut>S. Engholm</aut>, <vrk>Homoj sur la tero</vrk>, 
        <lok>1932</lok></fnt>.
      </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="sent.sen0a.MED">
      <dif>
        Ne havanta <tld/>on korpan:
      <ekz>
        miaj fingroj estis sen<tld/>aj, kaj mia nazo gutis pro 
        la malvarmo
        <fnt><aut>A. L&ouml;wenstein</aut>, <vrk>La &scirc;tona urbo</vrk>, 
        <lok>1999</lok></fnt>.
      </ekz>
      </dif>
    </subsnc>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_ch;&c_u;&c_l;&c_mol;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_e;&c_m;&c_a;&c_c;&c_y;&c_j;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>unfeeling</trd>,
    <trd>insensitive</trd>, <trd>insensate</trd>
  </trdgrp>
  <trd lng="fr">insensible<klr> (qui ne sent pas)</klr></trd>
  <trd lng="es">insensible</trd>
  <trdgrp lng="uk">
    <trd>&c_n;&c_je;&c_ch;&c_u;&c_l;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_d;&c_u;&c_sh;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="sent.sen0eco">
  <kap>sen<tld/>eco</kap>
  <snc mrk="sent.sen0eco.PSI">
    <dif>
      Foresto de <tld/>oj animaj a&ubreve; korpaj:
      <ekz>
        multaj e&ccirc; ripro&ccirc;as al li la severecon de lia karaktero kaj 
        vidas en &gcirc;i signon de fiereco a&ubreve; de sen<tld/>eco
        <fnt><aut>H. Sienkewicz, trad. L. Zamenhof</aut>, <vrk>Quo 
        vadis?</vrk>, <lok>1933</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="indife.0eco">indiferenteco</ref>,
    <ref cel="dolor.sen0eco">sendoloreco</ref>,
    <ref cel="sven.0o">sveno</ref>
  </refgrp>
   <trdgrp lng="en">
      <trd>insensitivity</trd>,
      <trd>insensibility</trd>
    </trdgrp>
  <trd lng="fr">insensibilit&eacute;</trd>
  <trd lng="es">insensibilidad</trd>
  <trd lng="nl">ongevoeligheid</trd>
</drv>

<drv mrk="sent.sen0igi">
  <kap>sen<tld/>igi</kap>  
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sent.sen0igi.KOMUNE">
    <dif>
      Senigi momente a&ubreve; da&ubreve;re je <tld/>opovo:
      <ekz>
        pliparton de tiuj homoj regis unu penso <klr>[&#8230;]</klr>, kiu jam 
        dum la vivo sen<tld/>igis ilin al &ccirc;io, kio okazis &ccirc;irka&ubreve; ili 
        <fnt><aut>H. Sienkewicz, trad. L. Zamenhof</aut>, <vrk>Quo 
        vadis?</vrk>, <lok>1933</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="aneste.0i">anestezi</ref>
  </refgrp>
  <trd lng="en">desensitize</trd>
  <trdgrp lng="fr">
    <trd>couper toute <ind>&eacute;motion</ind> &agrave;</trd>,
    <trd>insensibiliser</trd>
  </trdgrp>
  <trd lng="es">insensibilizar</trd>
  <trd lng="sk">znecitlivi&tcaron;</trd>
  <trd lng="cs">znecitliv&ecaron;t</trd>
</drv>

<drv mrk="sent.tro0ema">
  <kap>tro<tld/>ema</kap>
  <snc mrk="sent.tro0ema.PSI">
    <dif>
      Kiu tro intense, tro emocie, tre perturbe <tld/>as:
      <ekz>
        al la tro<tld/>ema humoro de sinjoro <nom>Lupp&aacute;n</nom> forte efikis
        &ccirc;i tiu evento, kaj li falis en altan febron
        <fnt>
          <aut>K. Miksz&aacute;th, trad. J. Horvath</aut>
          <vrk><url ref="http://vortaro.hu/mindenesszotar/jozohor/Fraulino%20el%20oro.pdf">
          Fra&ubreve;lino el oro</url></vrk>
          <lok>2002</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">hypersensible</trd>
  <trdgrp lng="uk">
    <trd>&c_n;&c_a;&c_d;&c_t;&c_o; &c_ch;&c_u;&c_t;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_je;&c_n;&c_t;&c_i;&c_m;&c_je;&c_n;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
