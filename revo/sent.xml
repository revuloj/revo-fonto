  <trd lng="et">tunda</trd>
  <trd lng="eu">sentitzen</trd>
  <trdgrp lng="fr">
    <trd>&eacute;prouver</trd>,
    <trd>sentir</trd>
  </trdgrp>
  <trd lng="fy">fiele</trd>
  <trd lng="gl">sentir</trd>
  <trd lng="gu">&#xAB2;&#xABE;&#xA97;&#xAC7;</trd>
  <trd lng="ha">ji</trd>
