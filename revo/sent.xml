</drv>

<drv mrk="sent.0a">
  <kap><tld/>a</kap>
  <snc mrk="sent.0a.KOMUNE">
    <dif>
      Rilata al la <tld/>oj:
      <ekz>
        vidado kaj a&ubreve;dado &ccirc;iam estis rigardataj de la homo
        kiel la plej gravaj <tld/>aj kapabloj
        <fnt><bib>Fab3</bib>, <lok>&scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>.
      </ekz>
      <ekz>
        <tld/>a esprimo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_ch;&c_u;&c_c;&c_mol;&c_c;&c_jo;&c_v;&c_y;</trd>,
    <trd>&c_a;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>sensory</trd>,
    <trd>perceptual</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>sensitivo</trd>,
    <trd>sentido</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>sensible<klr> (relevant des sens)</klr></trd>,
    <trd>sentimental<klr> (relevant des sentiments)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;rz&eacute;s-</trd>,
    <trd>&eacute;rzelmi</trd>,
    <trd>&eacute;rz&eacute;kel&odblac;-</trd>
  </trdgrp>
  <trd lng="nl">gevoels-</trd>
  <trd lng="no">f&oslash;lelses-</trd>
  <trd lng="pl">odczuwalny</trd>
  <trd lng="ru">&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="sent.0o">
  <kap><tld/>o</kap>
  <snc mrk="sent.0o.kapablo">
    <dif>
      Kapablo <tld/>i korpe a&ubreve; morale; <tld/>okapablo
      &gcirc;enerala:
      <ekz>
        Esperanto estas filo ne de logika spirito, sed de agema <tld/>o.
      </ekz>
    </dif>
    <trd lng="en">perception</trd>
    <trd lng="pl">czucie</trd>
  </snc>
  <snc mrk="sent.0o.impreso">
    <dif>
      Agrabla a&ubreve; malagrabla stato de la konscio, estigita de
      fizikaj ka&ubreve;zoj
    </dif>
    <refgrp tip="sub">
      <ref cel="dolor.0o">doloro</ref>,
      <ref cel="gxu">&gcirc;uo</ref>,
      <ref cel="plezur.0o">plezuro</ref>,
      <ref cel="sufer">sufero</ref>,
      <ref cel="volupt.0o">volupto</ref>
    </refgrp>
    <trdgrp lng="en">
      <trd>feeling</trd>,
      <trd>sensation</trd>
    </trdgrp>
    <trd lng="hu">&eacute;rz&eacute;kel&eacute;s</trd>
    <trdgrp lng="pl">
      <trd>odczucie</trd>,
      <trd>poczucie</trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_shch;&c_u;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="sent.0o.emocio">
    <dif>
      Agrabla a&ubreve; malagrabla stato de la konscio, estigita de
      nemateriaj moralaj ka&ubreve;zoj (
      <refgrp tip="ekz">
        <ref cel="gxoj.0o">&gcirc;ojo</ref>, 
        <ref cel="gxoj.mal0o">mal&gcirc;ojo</ref>,
        <ref cel="enu.0o">enuo</ref>, 
        <ref cel="mok.0o">moko</ref>, 
        <ref cel="hont.0o">honto</ref>, 
        <ref cel="fier.0eco">fiereco</ref>, 
        <ref cel="surpri.0o">surprizo</ref>,
        <ref cel="tim.0o">timo</ref>, 
        <ref cel="bedaux.0o">beda&ubreve;ro</ref>, 
        <ref cel="admir.0o">admiro</ref>, 
        <ref cel="am.0o">amo</ref>, 
        <ref cel="am.mal0o">malamo</ref> 
       </refgrp> ktp.); 
       <ref tip="sin" cel="sentim.0o.FIL">sentimento <sncref/></ref>:
      <ekz>
        maldol&ccirc;a <tld/>o de neplenumita espero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li deziris komuniki al &ccirc;iuj la <tld/>ojn, kiuj plenigis
        lian animon;
      </ekz>
      <ekz>
        tiam strangaj <tld/>oj enigas nian animon;
      </ekz>
      <ekz>
        liaj <tld/>oj el&scirc;prucas en lia parolmaniero;
      </ekz>
      <ekz>
        li estas tre sinretena en la esprimo de siaj <tld/>oj;
      </ekz>
      <ekz>
        iliaj paroloj estas kontra&ubreve;aj al iliaj propraj
        <tld/>oj<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="afekci.0o">afekcio</ref>,
      <ref cel="emoci.0o">emocio</ref>,
      <ref cel="vibr.0i">vibri</ref>
    </refgrp>
    <trdgrp lng="en">
      <trd>sense</trd>,
      <trd>sentiment</trd>, <trd>feeling</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>czucie</trd>,
      <trd>uczucie</trd>,
      <trd>odczucie</trd>,
      <trd>poczucie</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_e;&c_m;&c_o;&c_c;&c_i;&c_ja;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_je;&c_zh;&c_i;&c_v;&c_a;&c_n;&c_i;&c_je;
      </trd>
    </trdgrp>
  </snc>
  <snc mrk="sent.0o.inklino">
    <dif>
      Ama a&ubreve; malama inklino al iu; kor<tld/>o:
      <ekz>
        esprimi al iu la <tld/>ojn, kiujn oni havas por li<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kor.0o.FIG">koro <sncref/></ref>,
      <ref cel="pasi.0o">pasio</ref>,
      <ref cel="simpat.0o">simpatio</ref>
    </refgrp>
    <trd lng="en">feeling</trd>
    <trdgrp lng="pl">
      <trd>odczucie</trd>,
      <trd>poczucie</trd>
    </trdgrp>
  </snc>
  <lstref lst="voko:sentoj">sentoj</lstref>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_ch;&c_u;&c_c;&c_mol;&c_c;&c_jo;</trd>,
    <trd>&c_a;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_e;&c_m;&c_o;&c_c;&c_y;&c_ja;</trd>,
    <trd>
    &c_p;&c_je;&c_r;&c_a;&c_zh;&c_y;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;
    </trd>
  </trdgrp>
  <trd lng="bo">&#xF5A;&#xF7C;&#xF62;&#xF0B;&#xF56;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>cit</trd>,
    <trd>citlivost</trd>,
    <trd>c&iacute;t&ecaron;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Gef&uuml;hl</trd>
  <trdgrp lng="es">
    <trd>sentimiento</trd>,
    <trd>sensaci&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>sentiment</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;rz&eacute;s</trd>,
    <trd>&eacute;rzelem</trd>
  </trdgrp>
  <trd lng="nl">gevoel</trd>
  <trd lng="no">f&oslash;lelse</trd>
  <trd lng="ru">&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>cit</trd>,
    <trd>pocit</trd>
  </trdgrp>
</drv>

<drv mrk="sent.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="sent.0ajxo.KOMUNE">
    <dif>
     <ref tip="dif" cel="sent.0umajxo"><tld lit="S"/>uma&jcirc;o</ref>:
     <ekz>
        li asertis, ke tio estis la plej terura nokto de lia vivo; por
        la plej granda sumo da mono li ne volus ankora&ubreve; unu fojon
        havi tian <tld/>a&jcirc;on
        <fnt><bib>Fab1</bib>, <lok>galo&scirc;oj de feli&ccirc;o</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="en">
    <trd>feeling</trd>,
    <trd>sensation</trd>
  </trdgrp>
  <trd lng="fr">sensation</trd>
  <trd lng="hu">&eacute;rzet</trd>
  <trd lng="nl">zintuiglijke <ind>waarneming</ind></trd>
  <trd lng="pl">czucie</trd>
  <trd lng="ru">&c_o;&c_shch;&c_u;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="es">sensaci&oacute;n</trd>
</drv>

<drv mrk="sent.0ebla">
  <kap><tld/>ebla</kap>
  <snc mrk="sent.0ebla.KOMUNE">
    <dif>
      Sufi&ccirc;e intensa, klara por esti <tld/>ata:
      <ekz>
        atako kontra&ubreve; la elektronikaj &scirc;altcentraloj de
        &scirc;tato efikas multe pli dramajn kaj pli longe <tld/>eblajn
        rezultojn
        <fnt><aut>S. Maul</aut>, <vrk>Militado sen bataliloj</vrk>,
        <bib>Monato</bib>, <lok>2001:1, p.
        6a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_y;
  </trd>
  <trdgrp lng="cs">
    <trd>citeln&yacute;</trd>,
    <trd>patrn&yacute;</trd>,
    <trd>poc&iacute;titeln&yacute;</trd>
  </trdgrp>
  <trd lng="en">perceptible</trd>
  <trd lng="es">sensible</trd>
  <trdgrp lng="fr">
    <trd>perceptible</trd>,
    <trd>sensible<klr> (perceptible)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;rezhet&odblac;</trd>,
    <trd>&eacute;rz&eacute;kelhet&odblac;</trd>
  </trdgrp>
  <trd lng="nl">merkbaar</trd>
  <trd lng="no">merkbar</trd>
  <trdgrp lng="pl">
    <trd>odczuwalny</trd>,
    <trd>wyczuwalny</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_shch;&c_u;&c_t;&c_i;&c_m;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>cite&lcaron;n&yacute;</trd>,
    <trd>v&yacute;razn&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="sent.0eble">
  <kap><tld/>eble</kap>
  <snc mrk="sent.0eble.KOMUNE">
    <dif>
      Sufi&ccirc;e intense, rapide por ke oni rimarku:
      <ekz>
        &scirc;ajnas al mi, ke dum la nuna jarcento <tld/>eble
        komenci&gcirc;is kriza dekadenco de la angla lingvo
        <fnt><aut>W. Auld</aut>, <vrk>Dekadenco de lingvo</vrk>,
        <bib>Monato</bib>,
        <lok>1996:2, p. 21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_a;
  </trd>
  <trd lng="en">perceptibly</trd>
  <trdgrp lng="fr">
    <trd>sensiblement</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;rezhet&odblac;en</trd>,
    <trd>&eacute;rz&eacute;kelhet&odblac;en</trd>
  </trdgrp>
  <trd lng="nl">merkbaar</trd>
  <trd lng="no">merkbar</trd>
  <trdgrp lng="pl">
    <trd>odczuwalnie</trd>,
    <trd>wyczuwalnie</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_shch;&c_u;&c_t;&c_i;&c_m;&c_o;</trd>
  <trd lng="es">sensiblemente</trd>
</drv>

<drv mrk="sent.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="sent.0ema.KOMUNE">
    <dif>
      Facile impresebla:
      <ekz>
        tiuj misteraj sonoj, ne a&ubreve;deblaj por la orelo, estas
        <tld/>eblaj por &ccirc;iu animo <tld/>ema<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_u;&c_l;&c_mol;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_ch;&c_u;&c_l;&c_y;</trd>,
    <trd>&c_u;&c_r;&c_a;&c_z;&c_mol;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bolestiv&yacute;</trd>,
    <trd>citliv&yacute;</trd>
  </trdgrp>
  <trd lng="en">sensitive</trd>
  <trd lng="es">sensible</trd>
  <trdgrp lng="fr">
    <trd>impressionnable<klr> (sensible)</klr></trd>,
    <trd>sensible<klr> (capable de sentir)</klr></trd>
