<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: pedolo1.xml,v 1.2 2023/02/10 18:32:18 afido Exp $">
    <kap>
      <rad>pedologi</rad>/o
      <fnt><bib>PIV2</bib></fnt>
    </kap>

<drv mrk="pedolo1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SCI</uzo><uzo tip="fak">EKOL</uzo>
  <snc mrk="pedolo1.0o.grundoscienco">
    <dif>
      <ref tip="dif" cel="grund.0oscienco">Grundoscienco</ref>:
      <ekz>
        tipajn stadiojn de la grundevoluo oni en la <tld/>o
        klasifikas kiel grundotipoj<klr>n</klr>
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.pandapedia.com/wiki/Pedogenezo">Pedogenezo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Pedologie</trd>
    <trd lng="nl">bodemkunde</trd>
    <trd lng="pl">pedologia</trd>
    <trd lng="ru">&c_p;&c_o;&c_ch;&c_v;&c_o;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <rim>
    Pri la homonimeco kun
    <ref tip="hom" cel="pedolo.0o.infanscienco">pedologio (infanscienco)</ref>
    vd la <ref tip="super" cel="pedolo.0o.rim">rimarkon tie</ref>.
  </rim>
</drv>

  </art>

