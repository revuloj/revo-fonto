    <trd>prendre</trd>,
    <trd>saisir<klr tip="ind"> (attraper)</klr></trd>
  </trdgrp>
  <trd lng="fy">fange</trd>
  <trd lng="gl">captura</trd>
  <trd lng="gu">&#xA95;&#xAC7;&#xA9A;</trd>
  <trd lng="ha">kama</trd>
