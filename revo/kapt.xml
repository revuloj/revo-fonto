  </trdgrp>
  <trd lng="co">caccia</trd>
  <trdgrp lng="cs">
    <trd>chytit</trd>,
    <trd>dopadnout</trd>,
    <trd>lapit</trd>,
    <trd>polapit</trd>,
    <trd>p&rcaron;istihnout</trd>,
    <trd>p&rcaron;istihnout</trd>,
    <trd>zachytit</trd>,
    <trd>zadr&zcaron;et</trd>,
    <trd>zajmout</trd>
  </trdgrp>
  <trd lng="da">fangst</trd>
  <trd lng="de">einfangen</trd>
  <trd lng="en">catch</trd>
  <trd lng="es">capturar</trd>
  <trd lng="et">saak</trd>
  <trdgrp lng="fr">
