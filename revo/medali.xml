<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: medali.xml,v 1.22 2024/01/25 20:10:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>medalion</rad>/o
</kap>
<drv mrk="medali.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="medali.0o.BELA_skatola">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Juvelo en formo de plata malfermebla skatoleto, ordinare portata
      per
      &ccirc;eno &ccirc;e la kolo kaj en kiu oni konservas portreton, harojn kaj
      ceterajn.
    </dif>
    <trd lng="en">locket</trd>
    <trd lng="pl">medalion</trd>
    <trd lng="pt">medalh&atilde;o <klr>(j&oacute;ia em forma de caixa)</klr></trd>
  </snc>
  <snc mrk="medali.0o.BELA_bareliefa">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Rondforma a&ubreve; ovala bareliefo kun figura&jcirc;o:
      <ekz>
        bronza <tld/>o pri la mortinto
        <fnt><bib>Kiso</bib>, <lok>tombeja arabesko</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="en">medallion</trd>
    <trd lng="pl">medalion</trd>
    <trdgrp lng="pt">
      <trd>medalh&atilde;o <klr>(baixo-relevo ornamental)</klr></trd>,
      <trd>medalha</trd>
    </trdgrp>
  </snc>
  <refgrp tip="vid">
    <ref cel="medal.0o">medalo</ref>
  </refgrp>
  <trd lng="be">&c_m;&c_e;&c_d;&c_a;&c_l;&c_mol;&c_jo;&c_n;</trd>
  <trd lng="ca">medall&oacute;</trd>
  <trd lng="de">Medaillon</trd>
  <trd lng="hu">medalion</trd>
  <trd lng="fr">m&eacute;dallion</trd>
  <trdgrp lng="ja">
    <trd>&#x30ED;&#x30B1;&#x30C3;&#x30C8;  <pr>&#x308D;&#x3051;&#x3063;&#x3068;</pr></trd>,
    <trd>&#x30E1;&#x30C0;&#x30A4;&#x30E8;&#x30F3;</trd>
  </trdgrp>
  <trd lng="nl">medaillon</trd>
  <trd lng="ro">medalion</trd>
  <trd lng="ru">&c_m;&c_je;&c_d;&c_a;&c_l;&c_mol;&c_o;&c_n;</trd>
  <trd lng="sk">medail&oacute;n</trd>
  <trdgrp lng="cs">
    <trd>medailon</trd>,
    <trd>medailonek</trd>
  </trdgrp>
  <trd lng="uk">&#x43C;&#x435;&#x434;&#x430;&#x43B;&#x44C;&#x439;&#x43E;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x5C0F;&#x76D2; <pr>xi&#x1CE;oh&#xE9;</pr></trd>,
    <trd>&#x7D00;&#x5FF5;&#x54C1;&#x76D2; <pr>j&#xEC;ni&#xE0;np&#x1D0;nh&#xE9;</pr></trd>,
    <trd>&#x7EAA;&#x5FF5;&#x54C1;&#x76D2; <pr>j&#xEC;ni&#xE0;np&#x1D0;nh&#xE9;</pr></trd>
  </trdgrp>
</drv>
</art>
