<art mrk="$Id: estoni.xml,v 1.16 2023/11/07 20:34:12 revo Exp $">
<kap>
  <rad>Estoni</rad>/o
</kap>

<drv mrk="estoni.E0o">
  <kap><tld/>o</kap>
  <snc mrk="estoni.E0o.sxtato">
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
  <dif>
      <ref tip="dif" cel="eston.E0ujo">Estonujo</ref>:
      <ekz>
        <tld lit="E"/>o estas lando en nordorienta E&ubreve;ropo apud la
        Balta maro.
        <fnt><bib>Viki</bib>, <lok><url ref="&Viki;/Estonio">Estonio</url></lok>:</fnt>.
      </ekz>
      <ekz>
        jam de jaroj da&ubreve;ras inter <tld/>o kaj Rusio politikaj intertraktadoj
        <fnt>
           <aut>Helju Viires</aut>      
          <vrk><url ref="&Monato;/1993/A930901.php">Hejmen la rusoj ...?</url></vrk>
          <bib>Monato</bib>

          <lok>1993/09, p. 9</lok>
        </fnt>;
      </ekz>
      <ekz>
        plej bonajn rezultojn denove prezentis <tld/>o, kies indico de ekonomia libereco
        en la jaro 1997 estis 6,8
        <fnt>
       <aut>LAST</aut>
          <vrk><url ref="&Monato;/2000/006855.php">Ekonomia libereco</url></vrk>
          <bib>Monato</bib>
          <lok>2000/05, p. 7</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_E;&c_s;&c_t;&c_o;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_Je;&c_s;&c_t;&c_o;&c_n;&c_i;&c_ja;</trd>
  <trd lng="cs">Estonsko</trd>
  <trd lng="da">Estland</trd>
  <trd lng="de">Estland</trd>
  <trd lng="en">Estonia</trd>
  <trdgrp lng="fr">
    <trd>Estonie</trd>,
    <trd>R&eacute;publique d'Estonie</trd>
  </trdgrp>
  <trd lng="id">Estonia</trd>
  <trd lng="ja">&#x30A8;&#x30B9;&#x30C8;&#x30CB;&#x30A2;</trd>
  <trd lng="nl">Estland</trd>
  <trd lng="no">Estland</trd>
  <trd lng="pl">Estonia</trd>
  <trd lng="sk">Est&oacute;nsko</trd>
  <trd lng="sv">Estland</trd>
  <trd lng="uk">&c_Je;&c_s;&c_t;&#x43E;&#x43D;&c_ib;&#x44F;</trd>
  <trd lng="zh">&#x611B;&#x6C99;&#x5C3C;&#x4E9E; <pr>&#xE0;ish&#x101;n&#xED;y&#xE0;</pr></trd>
</drv>
</art>

