<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tril</rad>/o
</kap>
<drv mrk="tril.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Rapida ripetado de du sonoj, kiujn disigas nur tono a&ubreve; duontono.
    </dif>
  </snc>
  <trd lng="de">Triller</trd>
  <trd lng="nl">triller</trd>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_l;&c_mol;</trd>
</drv>

<drv mrk="tril.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      A&ubreve;digi <tld/>ojn:
      <ekz>
        la ala&ubreve;do <tld/>adis<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">trillern</trd>
  <trd lng="nl">trillers zingen</trd>
</drv>
</art>
