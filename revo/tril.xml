<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tril</rad>/o
</kap>
<drv mrk="tril.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="ornam.melodi0o">Melodiornamo</ref>
      uzata en klasika e&ubreve;ropa muziko,
      kiu da&ubreve;re, tre rapide alternigas la tonon kun alia tono
      duon- a&ubreve; plendute diferenca.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_e;&c_l;&c_mol;</trd>
  <trd lng="de">Triller</trd>
  <trd lng="hu">trilla</trd>
  <trdgrp lng="fr">
    <trd>trille</trd>
  </trdgrp>
  <trd lng="nl">triller</trd>
  <trdgrp lng="pt">
    <trd>trilo</trd>,
    <trd>quebro repetido</trd>,
    <trd>trinado</trd>,
    <trd>trino</trd>        
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_l;&c_mol;</trd>
</drv>

<drv mrk="tril.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      A&ubreve;digi <tld/>ojn:
      <ekz>
        la ala&ubreve;do <tld/>adis<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">trillern</trd>
  <trdgrp lng="fr">
    <trd>triller</trd>
  </trdgrp>
  <trd lng="hu">trill&aacute;zik</trd>
  <trd lng="nl">trillers zingen</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol; <ind>&c_t;&c_r;&c_je;&c_l;&c_i;</ind></trd>,
    <trd>&c_z;&c_a;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja; <ind>&c_t;&c_r;&c_je;&c_l;&c_mol;&c_ju;</ind></trd>
  </trdgrp>
</drv>
</art>
