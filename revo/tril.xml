<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tril.xml,v 1.25 2024/02/04 01:29:04 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tril</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tril.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tril.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="ornam.melodi0o">Melodiornamo</ref>
      kiu plurfoje rapide alternigas la tonon kun alia tono
      duon- a&ubreve; plendute diferenca:
      <ekz>
        li ornamis la melodion per perfekta aro de tiuj plibeliga&jcirc;oj
        kiujn sak&scirc;almistoj &scirc;atas kaj nomas &leftquot;<tld/>oj&rightquot;
        <fnt><aut>R. L. Stevenson, trad. A. Goodheir</aut>,
        <vrk>Forkaptita</vrk>, <lok>Kardo, 1976</lok></fnt>;
      </ekz>
      <ekz>
        Hel-tinte <tld/>e glitas &#11005; De l' rido gaja gamo
        <fnt>
          <bib>LSB</bib>
          <lok>Tentado</lok>
        </fnt>;
      </ekz>
      <ekz>
        li a&ubreve;dis gajajn <tld/>ojn de facilkoraj birdetoj
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        nigroblanka klavaro de la mekanika fortepiano kuras <klr>[&#8230;]</klr> obtuzigante
        la kunvenintojn per tondraj <tld/>oj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        birdoj fajfis sagacajn <tld/>ojn
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_e;&c_l;&c_mol;</trd>
  <trd lng="bg">&c_t;&c_r;&c_i;&c_l;&c_je;&c_r;</trd>
  <trd lng="ca">trinat</trd>
  <trd lng="cs">trylek</trd>
  <trd lng="de">Triller</trd>
  <trdgrp lng="en">
    <trd>trill</trd>,
    <trd>warble</trd>
  </trdgrp>
  <trd lng="fr">trille</trd>
  <trd lng="hu">trilla</trd>
  <trdgrp lng="ja">
    <trd>&#x30C8;&#x30EA;&#x30EB;</trd>,
    <trd>&#x986B;&#x97F3; <pr>&#12379;&#12435;&#12362;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">triller</trd>
  <trdgrp lng="pl">
    <trd>trel</trd>,
    <trd>tryl</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>trilo</trd>,
    <trd>quebro repetido</trd>,
    <trd>trinado</trd>,
    <trd>trino</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">trilok</trd>
  <trd lng="uk">&c_t;&c_r;&c_je;&c_l;&c_mol;</trd>
</drv>

<drv mrk="tril.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="tril.0i.MUZ">
    <dif>
      A&ubreve;digi <tld/>ojn:
      <ekz>
        a&ubreve;di&gcirc;is <tld/>ado de la najtingalo
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>as kanteton
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la violon&ccirc;elisto ekludas la saman melodion, ravige la sonoj de la unua parto
        <tld/>adas tra la &ccirc;ambro
        <fnt>
          <bib>IdO</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la paruoj, kiuj dum la tuta vintro pepis per ra&ubreve;kaj vo&ccirc;etoj, jam <tld/>etis
        klartone
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i kiel juna knabino
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        furioze <tld/>adis harmoniko
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxirp.0i">&ccirc;irpi</ref>,
      <ref cel="jodl.0i">jodli</ref>,
      <ref cel="sonor.0i.SON">sonori<sncref/></ref>,
      <ref cel="tint.0i.SON">tinti<sncref/></ref>,
      <ref cel="ulul.0i">ululi</ref>,
      <ref cel="vibr.0i.sono">vibri<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; <ind>&c_t;&c_r;&c_e;&c_l;&c_ib;</ind></trd>
  <trd lng="ca">trinar</trd>
  <trd lng="cs">&scaron;t&ecaron;betat</trd>
  <trdgrp lng="de">
    <trd>trillern</trd>,
    <trd>tr&auml;llern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>trill</trd>,
    <trd>warble</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>triller</trd>
  </trdgrp>
  <trd lng="hu">trill&aacute;zik</trd>
  <trdgrp lng="ja">
    <trd>&#x30C8;&#x30EA;&#x30EB;&#12391;&#x6B4C;&#12358;  <pr>&#x30C8;&#x30EA;&#x30EB;&#12391;&#12358;&#x305F;&#12358;</pr></trd>,
    <trd>&#x58F0;&#12434;&#38663;&#x308F;&#x305B;&#12390;&#12373;&#12360;&#x305A;&#x308B;
    <pr>&#12371;&#12360;&#12434;&#12405;&#x308B;&#x308F;&#x305B;&#12390;&#12373;&#12360;&#x305A;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">trillers zingen</trd>
  <trdgrp lng="pl">
    <trd>trelowa&cacute;</trd>,
    <trd>&sacute;piewa&cacute;  <ind>trelami</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol; <ind>&c_t;&c_r;&c_je;&c_l;&c_i;</ind></trd>,
    <trd>&c_z;&c_a;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja; <ind>&c_t;&c_r;&c_je;&c_l;&c_mol;&c_ju;</ind></trd>
  </trdgrp>
  <trd lng="sk">trilkova&tcaron;</trd>
</drv>
</art>
