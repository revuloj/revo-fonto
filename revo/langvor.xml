<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: langvor.xml,v 1.13 2022/05/01 12:04:34 revo Exp $">
<kap>
  <rad>langvor</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="langvor.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">POE</uzo>
  <snc mrk="langvor.0o.malviglo">
    <dif>
      <ref tip="super" cel="vigl.mal0o">Malviglo</ref>,
      &gcirc;enerala laco kun <ref tip="prt" cel="intere.ne0one0igxo">neintereso</ref>
      pri &ccirc;io ekstera:
      <ekz>
        dol&ccirc;a <tld/>o
        <fnt>
          <vrk><url ref="https://vanoeo.wordpress.com/2012/08/07/la-dolca-langvoro/">La Va&ccirc;onga Mondo: La dol&ccirc;a langvoro&#x2026;</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        vi ku&scirc;as en plena malstre&ccirc;i&gcirc;o kaj <tld/>o
        <fnt>
          <vrk><url ref="http://verkoj.com/verkistoj/valentin-melnikov/leterfragmento/">Leterfragmento</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="apati.0o">apatio</ref>,
      <ref cel="deprim.0o">deprimo</ref>,
      <ref cel="dorm.0ema">dormemo</ref>,
      <ref cel="enu.0o">enuo</ref>,
      <ref cel="indife.0eco">indiferenteco</ref>,
      <ref cel="melank.0o">melankolio</ref>,
      <ref cel="splen.0o">spleno</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_l;&c_ja;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_l;&c_o;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_n;&c_ja;&c_m;&c_o;&c_g;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">llangor</trd>
  <trdgrp lng="cs">
    <trd>mal&aacute;tnost</trd>,
    <trd>ochablost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Tr&auml;gheit</trd>,
    <trd>Mattigkeit</trd>
  </trdgrp>
  <trd lng="en">languor</trd>
  <trdgrp lng="fr">
    <trd>alanguissement</trd>,
    <trd>langueur</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>b&aacute;gyadts&aacute;g</trd>,
    <trd>ernyedts&eacute;g</trd>,
    <trd>lankad&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>loomheid</trd>,
    <trd>slapheid</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wyczerpanie</trd>,
    <trd>niemoc</trd>,
    <trd>zm&eogonek;czenie</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>epuizare</trd>,
    <trd>neputin&#x21B;&abreve;</trd>,
    <trd>oboseal&abreve;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mal&aacute;tnos&tcaron;</trd>,
    <trd>ochablos&tcaron;</trd>,
    <trd>skleslos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x441;&#x43C;&#x443;&#x442;&#x43E;&#x43A;</trd>,
    <trd>&#x43F;&#x435;&#x447;&#x430;&#x43B;&#x44C;</trd>,
    <trd>&#x43D;&#x443;&#x434;&#x44C;&#x433;&#x430;</trd>,
    <trd>&#x437;&#x43D;&#x435;&#x43C;&#x43E;&#x433;&#x430;</trd>,
    <trd>&#x437;&#x43D;&#x435;&#x441;&#x438;&#x43B;&#x435;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x437;&#x43D;&#x435;&#x441;&#x438;&#x43B;&#x43B;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="langvor.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">POE</uzo>
  <snc mrk="langvor.0i.malvigli">
    <dif>
      <ref tip="super" cel="vigl.mal0i">Malvigli</ref>, pro korpa kaj spirita laco sin lasi al iom malgaja
      revado:
      <ekz>
        la subaj uloj permesis al si jen <tld/>i, jen formi klikon
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 58a, p. 277a</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i longe rigardas min en silento, <tld/>e gustumante la kafon
        <fnt>
          <bib>Monato</bib>
          <aut>Daniel L. Mason</aut>
          <vrk><url ref="&Monato;/2011/010276.php">Matena kafo</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        Anstata&ubreve; pimpe Ala&ubreve;djubili al la lum' Ja licas nur sengrimpe <tld lit="L"/>i en
        mucida hum'
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
          <lok>Lamentado</lok>
        </fnt>;
      </ekz>
      <ekz>
        venos konscio pri la trista stato nia: ke vi kaj mi <tld/>as en katenoj for&gcirc;itaj
        pli per hom' ol per destino
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2010/009789.php">Soneto</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">llanguir</trd>
  <trdgrp lng="cs">
    <trd>b&yacute;t vy&ccaron;erp&aacute;n</trd>,
    <trd>sou&zcaron;it se</trd>,
    <trd>tr&aacute;pit se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>abh&auml;ngen <klr>(nichts tun)</klr></trd>,
    <trd><ind>Tr&uuml;bsal</ind> blasen</trd>
  </trdgrp>
  <trd lng="en">languish</trd>
  <trdgrp lng="fr">
    <trd>languir</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>b&aacute;gyadt</trd>,
    <trd>ernyedt</trd>,
    <trd>lankadt</trd>
  </trdgrp>
  <trd lng="nl">kwijnen</trd>
  <trdgrp lng="pl">
    <trd>wyczerpanym by&cacute;</trd>,
    <trd>zm&eogonek;czonym by&cacute;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>istovita</trd>,
    <trd>obosit</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>by&tcaron; vy&ccaron;erpan&yacute;</trd>,
    <trd>s&uacute;&zcaron;i&tcaron; sa</trd>,
    <trd>tr&aacute;pi&tcaron; sa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&#x443;&#x434;&#x438;&#x442;&#x438;&#x441;&#x44F;</trd>,
    <trd>&#x437;&#x43D;&#x435;&#x43C;&#x430;&#x433;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>
</art>

