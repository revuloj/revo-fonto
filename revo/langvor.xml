<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>langvor</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="langvor.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">POE</uzo>
  <snc>
    <dif>
      Malviglo, &gcirc;enerala laco kun neintereso pri &ccirc;io ekstera.
    </dif>
    <refgrp tip="vid">
      <ref cel="deprim.0o">deprimo</ref>,
      <ref cel="melank.0o">melankolio</ref>,
      <ref cel="splen.0o">spleno</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>alanguissement</trd>,
    <trd>langueur</trd>
  </trdgrp>
</drv>

<drv mrk="langvor.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">POE</uzo>
  <snc>
    <dif>
      Malvigli, pro korpa kaj spirita laco sin lasi al iom malgaja revado:
      <ekz>
        la subaj uloj permesis al si jen <tld/>i, jen formi klikon
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 58a, p. 277a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>languir</trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
-->
</vortaro>