<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>langvor</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="langvor.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">POE</uzo>
  <snc>
    <dif>
      Malviglo, &gcirc;enerala laco kun neintereso pri &ccirc;io ekstera.
    </dif>
    <refgrp tip="vid">
      <ref cel="deprim.0o">deprimo</ref>,
      <ref cel="melank.0o">melankolio</ref>,
      <ref cel="splen.0o">spleno</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_l;&c_ja;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_l;&c_o;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_n;&c_ja;&c_m;&c_o;&c_g;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">llangor</trd>
  <trdgrp lng="cs">
    <trd>mal&aacute;tnost</trd>,
    <trd>ochablost</trd>
  </trdgrp>
  <trd lng="en">languor</trd>
  <trdgrp lng="fr">
    <trd>alanguissement</trd>,
    <trd>langueur</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>b&aacute;gyadts&aacute;g</trd>,
    <trd>ernyedts&eacute;g</trd>,
    <trd>lankad&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>loomheid</trd>,
    <trd>slapheid</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wyczerpanie</trd>,
    <trd>niemoc</trd>,
    <trd>zm&eogonek;czenie</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>epuizare</trd>,
    <trd>neputin&#539;&abreve;</trd>,
    <trd>oboseal&abreve;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mal&aacute;tnos&tcaron;</trd>,
    <trd>ochablos&tcaron;</trd>,
    <trd>skleslos&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="langvor.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">POE</uzo>
  <snc>
    <dif>
      Malvigli, pro korpa kaj spirita laco sin lasi al iom malgaja
      revado:
      <ekz>
        la subaj uloj permesis al si jen <tld/>i, jen formi klikon
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 58a, p. 277a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">llanguir</trd>
  <trdgrp lng="cs">
    <trd>b&yacute;t vy&ccaron;erp&aacute;n</trd>,
    <trd>sou&zcaron;it se</trd>,
    <trd>tr&aacute;pit se</trd>
  </trdgrp>
  <trd lng="en">languish</trd>
  <trdgrp lng="fr">
    <trd>languir</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>b&aacute;gyadt</trd>,
    <trd>ernyedt</trd>,
    <trd>lankadt</trd>
  </trdgrp>
  <trd lng="nl">kwijnen</trd>
  <trdgrp lng="sk">
    <trd>by&tcaron; vy&ccaron;erpan&yacute;</trd>,
    <trd>s&uacute;&zcaron;i&tcaron; sa</trd>,
    <trd>tr&aacute;pi&tcaron; sa</trd>
  </trdgrp>
</drv>
</art>

