<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: erik1.xml,v 1.12 2023/11/07 20:34:11 revo Exp $">
<kap>
  <rad>Erik</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="erik1.0o">
  <kap><tld/>o</kap>
  <snc mrk="erik1.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        &scirc;i subite levis la kapon kaj rigardis <tld/>on
        <fnt><bib>InfanTorent2</bib>, <lok>dua parto, &ccirc;apitro
        1a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
     <ref tip="hom" cel="erik.0o">Eriko</ref> estas anka&ubreve; vegeta&jcirc;o.
  </rim>
  <trdgrp lng="be">
    <trd>&c_E;&c_r;&c_y;&c_h;</trd>,
    <trd>&c_E;&c_r;&c_y;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">&Egrave;ric</trd>
  <trd lng="de">Erik</trd>
  <trd lng="fr">&Eacute;ric</trd>
  <trdgrp lng="it">
    <trd>Enrico</trd>,
    <trd>Errico</trd>
  </trdgrp>
  <trd lng="nl">Erik</trd>
  <trd lng="pl">Eryk</trd>
  <trdgrp lng="pt">
    <trd>&Eacute;rico</trd>,
    <trd>Eric</trd>
  </trdgrp>
  <trd lng="uk">&#x415;&#x440;&#x438;&#x43A;</trd>
  <trd lng="zh">&#x57C3;&#x88E1;&#x514B; <pr>&#x101;il&#x1D0;k&#xE8;</pr></trd>
</drv>
</art>

