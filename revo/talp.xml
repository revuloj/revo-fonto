<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>talp</rad>/o
</kap>
<drv mrk="talp.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="talp.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Talpa</trd>), subtere vivanta insektoman&gcirc;ulo
      kun velureca felo:
      <ekz>
        blinda kiel <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Maulwurf</trd>
  <trd lng="en">mole</trd>
  <trd lng="fr">taupe</trd>
  <trd lng="nl">mol</trd>
</drv>
<drv mrk="talp.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo
      <ref tip="super" cel="mangx.insekto0uloj">insektoman&gcirc;uloj</ref>
      (<trd lng="la">Talpidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Maulw&uuml;rfe <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>
