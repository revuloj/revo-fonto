<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>talp</rad>/o
</kap>
<drv mrk="talp.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="talped.0oj"><tld/>edoj</ref>
      (<trd lng="la">Talpa</trd>), subtere vivanta insektoman&gcirc;ulo
      kun velureca felo:
      <ekz>
        blinda kiel <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_o;&c_t;</trd>
  <trd lng="de">Maulwurf</trd>
  <trd lng="en">mole</trd>
  <trd lng="fr">taupe</trd>
  <trd lng="hu">vakond</trd>
  <trd lng="nl">mol</trd>
  <trdgrp lng="pt">
   <trd>toupeira</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_r;&c_o;&c_t;</trd>
  <trd lng="sv">mullvad</trd>
</drv>
</art>
