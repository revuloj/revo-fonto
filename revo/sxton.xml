<drv mrk="sxton.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sxton.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Malmola pli malpli granda minerala maso:
      <ekz>
        sur la tero ku&scirc;as <tld/>o
        <fnt><bib>F</bib>, <lok>&FE; 6</lok></fnt>;
      </ekz>
      <ekz>
        ruli <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        Mi faros Jerusalemon amaso da <tld/>oj
        <fnt><bib>MT</bib>, <lok>&Jer; 9:10</lok></fnt>;
      </ekz>
      <ekz>
        alligi <tld/>on al &jcirc;etilo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        sa&gcirc;o estas pli bona ol multekostaj <tld/>oj
        <fnt><bib>MT</bib>, <lok>&Sen; 8:11</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj alportas sian <tld/>on <klr>(kunlaboras)</klr>
        por la konstruo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind>trafi du celojn per unu <tld/>o</ind>
        <trd lng="es">matar dos p&aacute;jaros de un tiro</trd>
        <trd lng="fr">faire d'une pierre deux coups</trd>
        <klr>(per sama rimedo havi du profitojn)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        per unu <tld/>o oni du &jcirc;etojn ne faras
        <klr>(por sukcesi oni ne devas samtempe celi plurajn
        atingota&jcirc;ojn;
        vd <ref tip="vid" cel="lepor.0o">leporo</ref>)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ne mi &jcirc;etis la unuan <tld/>on
        <klr>(ne mi unue atakis, iniciatis la malamikajn
        agojn)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &jcirc;eti <tld/>ojn sur la vojon de iu
        <klr>(intence aran&gcirc;i malfacila&jcirc;ojn al iu)</klr>;
      </ekz>
      <ekz>
        <ind><tld/>o de falpu&scirc;i&gcirc;o</ind>
        <klr>(falilo; ka&ubreve;zo de skandalo)</klr>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="be">&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;
        &c_s;&c_p;&c_a;&c_t;&c_y;&c_k;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_ja;
        </trd>
        <trd lng="es">escollo</trd>
        <trd lng="hu">botr&aacute;nyk&odblac;</trd>
        <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;
        &c_p;&c_r;&c_je;&c_t;&c_k;&c_n;&c_o;&c_v;&c_je;&c_n;&c_i;&c_ja;
        </trd>
      </ekz>
    </dif>
    <trd lng="bo">&t_ra;&t_data;&t_naro;&t_tsheg;</trd>
    <trd lng="en">stone</trd>
    <trd lng="es">piedra</trd>
    <trd lng="fr">pierre</trd>
    <trdgrp lng="it">
      <trd>pietra</trd>,
      <trd>sasso</trd>
    </trdgrp>
    <trd lng="pl">kamie&nacute;</trd>
  </snc>
  <snc mrk="sxton.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Negranda <tld/>eca
      <ref tip="super" cel="konkrement.0o.FIZL">konkremento</ref>
      kiu fojfoje formi&gcirc;as en glandoj a&ubreve; iliaj duktoj:
      <ekz>
        havi <tld/>on en reno, galveziko, salivglando.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxton.gal0amalsano">gal<tld/>a malsano</ref>,
      <ref cel="sxton.saliv0amalsano">saliv<tld/>a malsano</ref>,
      <ref cel="sxton.urin0amalsano">urin<tld/>a malsano</ref>
    </refgrp>
    <trd lng="en">calculus</trd>
    <trd lng="es">c&aacute;lculo<klr> (medicina)</klr></trd>
    <trd lng="fr">calcul<klr> (m&eacute;decine)</klr></trd>
    <trd lng="it">calcolo</trd>
    <trd lng="pl">kamie&nacute;</trd>
  </snc>
  <trd lng="af">klip</trd>
  <trd lng="am">&#x12F5;&#x1295;&#x130B;&#x12ED;</trd>
  <trd lng="ar">&#x62D;&#x62C;&#x631;</trd>
  <trd lng="az">da&#x15F;</trd>
