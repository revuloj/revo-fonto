<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>&scirc;ton</rad>/o <fnt><bib>PV</bib></fnt>
    </kap>

    <drv mrk="sxton.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="sxton.0o.MIN">
        <uzo tip="fak">MIN</uzo>
        <dif>
          Malmola pli malpli granda minerala maso:
          <ekz>
            sur la tero ku&scirc;as <tld/>o
            <fnt><bib>F</bib>, <lok>&FE; 6</lok></fnt>;
          </ekz><ekz>
            ruli <tld/>on<fnt>Z</fnt>;
          </ekz><ekz>
            Mi faros Jerusalemon amaso da <tld/>oj
            <fnt><bib>MT</bib>, <lok>&Jer; 9:10</lok></fnt>;
          </ekz><ekz>
            alligi <tld/>on al &jcirc;etilo<fnt>Z</fnt>;
          </ekz><ekz>
            sa&gcirc;o estas pli bona ol multekostaj <tld/>oj
            <fnt><bib>MT</bib>, <lok>&Sen; 8:11</lok></fnt>;
          </ekz><ekz>
            &ccirc;iuj alportas sian <tld/>on <klr>(kunlaboras)</klr>
            por la
            konstruo<fnt>Z</fnt>;
          </ekz><ekz>
            <ind>trafi du celojn per unu <tld/>o</ind>
            <trd lng="fr">faire d'une pierre deux coups</trd>
            <klr>(per sama rimedo havi du profitojn)</klr>
            <fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            per unu <tld/>o oni du &jcirc;etojn ne faras
            <klr>(por sukcesi oni ne devas samtempe celi plurajn
            atingota&jcirc;ojn;
              vd <ref tip="vid" cel="lepor.0o">leporo</ref>)</klr>
            <fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            ne mi &jcirc;etis la unuan <tld/>on
            <klr>(ne mi unue atakis, iniciatis la malamikajn
            agojn)</klr>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            &jcirc;eti <tld/>ojn sur la vojon de iu
            <klr>(intence aran&gcirc;i malfacila&jcirc;ojn al iu)</klr>;
          </ekz><ekz>
            <ind><tld/>o de falpu&scirc;i&gcirc;o</ind>
            <klr>(falilo; ka&ubreve;zo de skandalo)</klr>
            <fnt><bib>PrV</bib></fnt>.
            <trd lng="be">&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;
              &c_s;&c_p;&c_a;&c_t;&c_y;&c_k;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_ja;</trd>
            <trd lng="hu">botr&aacute;nyk&odblac;</trd>
            <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;
              &c_p;&c_r;&c_je;&c_t;&c_k;&c_n;&c_o;&c_v;&c_je;&c_n;&c_i;&c_ja;</trd>
          </ekz>
        </dif>
        <trd lng="en">stone</trd>
        <trd lng="pl">kamie&nacute;</trd>
      </snc>
      <snc mrk="sxton.0o.MED">
        <uzo tip="fak">MED</uzo>
        <dif>
          Negranda <tld/>o<sncref ref="sxton.0o.MIN"/> kiu fojfoje
          formi&gcirc;as en glandoj a&ubreve; iliaj duktoj:
          <ekz>
            havi <tld/>on en reno, galveziko, salivglando.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="sxton.gal0amalsano">gal<tld/>a malsano</ref>,
          <ref cel="sxton.saliv0amalsano">saliv<tld/>a malsano</ref>,
          <ref cel="sxton.urin0amalsano">urin<tld/>a malsano</ref>
        </refgrp>
        <trd lng="en">calculus</trd>
        <trd lng="pl">kamie&nacute;</trd>
      </snc>
      <trd lng="be">&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;</trd>
      <trd lng="de">Stein</trd>
      <trd lng="fr">pierre</trd>
      <trd lng="hu">k&odblac;</trd>
      <trd lng="nl">steen</trd>
      <trd lng="pt">pedra</trd>
      <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;</trd>
      <trd lng="sv">sten</trd>
      <trd lng="tp">kiwen</trd>
    </drv>

    <drv mrk="sxton.0a">
      <kap><tld/>a</kap>
      <snc mrk="sxton.0a.MIN">
        <dif>
          El <tld/>o:
          <ekz>
            mi dezirus lo&gcirc;i en granda <tld/>a kastelo
            <fnt><bib>ElektFab</bib>, <lok>fi&scirc;kaptisto kaj lia
                edzino</lok></fnt>.
          </ekz>
        </dif>
        <trd lng="pl">kamienny</trd>
      </snc>
      <snc mrk="sxton.0a.FIG_fortika">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Fortika nevenkebla:
          <ekz>
            ili dormis <tld/>an dormon<fnt>B</fnt>
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="fer.0a.FIG">fera <sncref/></ref>,
          <ref cel="sxtal.0a.FIG">&scirc;tala <sncref/></ref>
        </refgrp>
        <trd lng="pl">kamienny</trd>
      </snc>
      <snc mrk="sxton.0a.FIG_senindulga">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Ne mildigebla, senkompata, senindulga, senama:
          <ekz>
            koro <tld/>a<fnt>Z</fnt>
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="marmor">marmora</ref>
        </refgrp>
        <trd lng="pl">kamienny</trd>
      </snc>
      <trd lng="be">&c_k;&c_a;&c_m;&c_je;&c_n;&c_n;&c_y;</trd>
      <trdgrp lng="fr">
        <trd>lithique</trd>,
        <trd>de <ind>pierre</ind></trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>k&odblac;-</trd>,
        <trd>k&odblac;kem&eacute;ny</trd>,
        <trd>k&odblac;b&odblac;l val&oacute;</trd>
      </trdgrp>
      <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
      <trd lng="tp">kiwen</trd>
    </drv>

    <drv mrk="sxton.0ego">
      <kap><tld/>ego</kap>
      <snc mrk="sxton.0ego.MIN">
        <uzo tip="fak">MIN</uzo>
        <dif>
          Granda nemovebla <tld/>o:
          <ekz>
            tombo <klr>...</klr> elhakita el <tld/>ego
            <fnt><bib>NT</bib>, <lok>&Mar; 15:46</lok></fnt>.
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="rok.0o">roko</ref>
        </refgrp>
      </snc>
      <trdgrp lng="be">
        <trd>&c_u;&c_c;&c_jo;&c_s;</trd>,
        <trd>&c_s;&c_k;&c_a;&c_l;&c_a;</trd>,
        <trd>&c_v;&c_a;&c_l;&c_u;&c_n;</trd>,
        <trd>&c_k;&c_a;&c_m;&c_l;&c_y;&c_g;&c_a;</trd>
      </trdgrp>
      <trd lng="de">Fels</trd>
      <trd lng="fr">roc</trd>
      <trd lng="hu">k&odblac;szikla</trd>
      <trd lng="nl">rots</trd>
      <trdgrp lng="pl">
        <trd>g&lstroke;az</trd>,
        <trd>ska&lstroke;a</trd>,
        <trd>kamol<klr> (pot.)</klr></trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_u;&c_t;&c_jo;&c_s;</trd>,
        <trd>&c_s;&c_k;&c_a;&c_l;&c_a;</trd>
      </trdgrp>
    </drv>

    <drv mrk="sxton.0eto">
      <kap><tld/>eto</kap>
      <snc mrk="sxton.0eto.MIN">
        <uzo tip="fak">MIN</uzo>
        <dif>
          Tre malgranda <tld/>o:
          <ekz>
            <uzo tip="stl">FIG</uzo>
            lia bu&scirc;o estas plena de <tld/>etoj<fnt>Z</fnt>
            <klr>(li malklare prononcas)</klr>.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="gruz.0o">gruzo</ref>,
          <ref cel="sabl.0o">sablo</ref>
        </refgrp>
      </snc>
      <trd
      lng="be">&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;&c_ch;&c_y;&c_k;</trd>
      <trd lng="fr">caillou</trd>
      <trd lng="hu">kavics</trd>
      <trd lng="nl">kiezelsteen</trd>
      <trdgrp lng="pl">
        <trd>kamyk</trd>,
        <trd>kamyczek</trd>
      </trdgrp>
      <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_sh;&c_je;&c_k;</trd>
    </drv>

    <drv mrk="sxton.0ejo">
      <kap><tld/>ejo, <var><kap><tld/>rompejo</kap></var>, 
        <var><kap><tld/>minejo</kap></var></kap>
      <snc>
        <dif>
          Loko kie oni ekspluatas <tld/>ojn:
          <ekz>
            mi memoros viajn vortojn, dum mi rompos rokojn en
            <tld/>minejo
            <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;apitro
            30a</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd
      lng="be">&c_k;&c_a;&c_m;&c_je;&c_n;&c_ja;&c_l;&c_o;&c_m;&c_n;&c_ja;</trd>
      <trd lng="de">Steinbruch</trd>
      <trd lng="fr">carri&egrave;re de pierre</trd>
      <trdgrp lng="hu">
        <trd>k&odblac;t&ouml;r&odblac;</trd>,
        <trd>k&odblac;fejt&odblac;</trd>,
        <trd>k&odblac;b&aacute;nya</trd>
      </trdgrp>
      <trd lng="nl">steengroeve</trd>
      <trdgrp lng="pl">
        <trd>kamienio&lstroke;om</trd>,
        <trd>wyrobisko <ind>kamieni</ind></trd>
      </trdgrp>
      <trd lng="pt">pedreira</trd>
      <trd
      lng="ru">&c_k;&c_a;&c_m;&c_je;&c_n;&c_o;&c_l;&c_o;&c_m;&c_n;&c_ja;</trd>
      <trd lng="sv">stenbrott</trd>
    </drv>

    <drv mrk="sxton.0igxi">
      <kap><tld/>i&gcirc;i</kap>
      <snc mrk="sxton.0igxi.MIN">
        <dif>
          I&gcirc;i <tld/>a, simila al <tld/>o.
        </dif>
        <trdgrp lng="hu">
          <trd>elk&odblac;vesedik</trd>,
          <trd>k&odblac;v&eacute; v&aacute;lik;</trd>
        </trdgrp>
        <trdgrp lng="pl">
          <trd>skamienie&cacute;</trd>,
          <trd>obraca&cacute; si&eogonek; w
          <ind>kamie&nacute;</ind></trd>,
          <trd>zmienia&cacute; si&eogonek; w
          <ind>kamie&nacute;</ind></trd>
        </trdgrp>
      </snc>
      <snc mrk="sxton.0igxi.FIG">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Rigidi&gcirc;i, senmovigi, i&gcirc;i neprogrespova:
          <ekz>
            &scirc;ia viza&gcirc;o <klr>...</klr> aspektis kiel
            <tld/>i&gcirc;inta
            <fnt><bib>Marta</bib></fnt>;
          </ekz><ekz>
            mi preska&ubreve; <tld/>i&gcirc;is &ccirc;e la ekvido de
            leono
            <fnt><bib>Munchhausen</bib>, <lok>&ccirc;apitro
            1a</lok></fnt>;
          </ekz><ekz>
            vivanta lingvo ne <tld/>i&gcirc;as;
          </ekz><ekz>
            homo kun <tld/>i&gcirc;intaj ideoj.
          </ekz>
        </dif>
        <trd lng="hu">k&odblac;v&eacute; dermed</trd>
        <trd lng="pl">skamienie&cacute;</trd>
      </snc>
      <trd
      lng="be">&c_a;&c_k;&c_a;&c_m;&c_ja;&c_n;&c_je;&c_c;&c_mol;</trd>
      <trd lng="fr">se <ind>p&eacute;trifier</ind></trd>
      <trd lng="nl">verstenen</trd>
      <trd
      lng="ru">&c_o;&c_k;&c_a;&c_m;&c_je;&c_n;&c_je;&c_t;&c_mol;</trd>
      <trd lng="sv">f&ouml;rstenas</trd>
    </drv>

    <drv mrk="sxton.0umi">
      <kap><tld/>umi</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="sxton.0umi.sxtonmortigi">
        <ref tip="dif" cel="mort.sxton0igi.KOMUNE"><tld/>mortigi</ref>
      </snc>
      <trd lng="be">&c_z;&c_a;&c_b;&c_ib;&c_c;&c_mol;
        <ind>&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;&c_n;&c_ja;&c_m;&c_ib;</ind>
        <klr>(&c_d;&c_a;
          &c_s;&c_mol;&c_m;&c_je;&c_r;&c_c;&c_ib;)</klr></trd>
      <trd lng="de">steinigen</trd>
      <trd lng="fr">lapider</trd>
      <trd lng="hu">megk&odblac;vez</trd>
      <trd lng="nl">stenigen</trd>
      <trd lng="pl">kamieniowa&cacute;</trd>
      <trd lng="pt">lapidar<klr tip="ind"> (matar)</klr></trd>
      <trd lng="ru">&c_p;&c_o;&c_b;&c_i;&c_v;&c_a;&c_t;&c_mol;
        &c_k;&c_a;&c_m;&c_n;&c_ja;&c_m;&c_i;</trd>
      <trd lng="sv">stena<klr> (till d&ouml;ds)</klr></trd>
    </drv>

    <drv mrk="sxton.sen0igi">
      <kap>sen<tld/>igi</kap>
      <snc mrk="sxton.sen0igi.AGR">
        <uzo tip="fak">AGR</uzo>
        <dif>
          Senigi je <tld/>oj, ekzemple kampon por faciligi kultivon:
          <ekz>
            vinber&gcirc;ardenon havis mia amiko <klr>...</klr> li
            &ccirc;irka&ubreve;fosis
            &gcirc;in,
            sen<tld/>igis &gcirc;in, kaj plantis en &gcirc;i plej
            bonspecajn
            vinberbran&ccirc;ojn
            <fnt><bib>MT</bib>, <lok>&Jes; 5:1-2</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="fr">&eacute;pierrer</trd>
      <trdgrp lng="pl">
        <trd>okdamieniowywa&cacute;</trd>,
        <trd>odgruzowywa&cacute;</trd>
      </trdgrp>
    </drv>

    <drv mrk="sxton.aer0o">
      <kap>aer<tld/>o</kap>
      <snc mrk="sxton.aer0o.aerolito">
        <uzo tip="fak">MET</uzo>
        <ref tip="dif" cel="aeroli.0o">aerolito</ref>
      </snc>
      <trd lng="br">aerolitenn</trd>
      <trdgrp lng="de">
        <trd>Meteorstein</trd>,
        <trd>Meteor</trd>,
        <trd>Aerolith</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>meteor</trd>,
        <trd>aerolite</trd>
      </trdgrp>
      <trd lng="fr">a&eacute;rolithe</trd>
      <trd lng="es">aerolito</trd>
      <trd lng="nl">a&euml;roliet</trd>
      <trdgrp lng="pl">
        <trd>aerolit</trd>,
        <trd>meteoryt</trd>,
        <trd>meteor</trd>
      </trdgrp>
      <trd lng="pt">aer&oacute;lito</trd>
      <trd lng="sk">meteorit</trd>
      <trdgrp lng="ru">
        <trd>&c_a;&c_e;&c_r;&c_o;&c_l;&c_i;&c_t;</trd>,
        <trd>&c_m;&c_je;&c_t;&c_je;&c_o;&c_r;&c_i;&c_t;</trd>
      </trdgrp>
    </drv>

    <drv mrk="sxton.filozofia0o">
      <kap>filozofia <tld/>o, <var><kap><tld/>o de
      sa&gcirc;uloj</kap></var></kap>
      <snc mrk="sxton.filozofia0o.MAG">
        <dif>
          Celo de <ref tip="vid" cel="alhxem.0o">al&hcirc;emio</ref>,
          la&ubreve;vorte 
          elemento kapabla &scirc;an&gcirc;i ordinarajn metalojn al oro:
          <ekz>
            li elkore pre&gcirc;is <klr>...</klr>, ke la atendata
            miraklo estu 
            nek la eltrovo de la filozofia <tld/>o, nek la liberigo de
            la 
            metal-viviga blovo
            <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de
            Diego</aut>, <vrk>Cent 
                Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="fr">pierre philosophale</trd>
      <trd lng="pl">kamie&nacute; filozoficzny</trd>
    </drv>

    <drv mrk="sxton.juvel0o">
      <kap>juvel<tld/>o</kap>
      <snc mrk="sxton.juvel0o.MIN">
        <tezrad fak="MIN"/>
        <uzo tip="fak">MIN</uzo>
        <dif>
          Multekosta <tld/>o uzata por fari juvelojn:
          <ekz>
            mi jam a&ubreve;dis la famon de via sa&gcirc;eco, kaj se
            &gcirc;i estas vere pli
            valora ol oro kaj juvel<tld/>o, ni &gcirc;ojos pri &gcirc;i
            <fnt><bib>SatirRak</bib>, <lok>Pythagoras, 4</lok></fnt>.
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="gem.0o">gemo</ref>
        </refgrp>
      </snc>
      <trd lng="be">&c_k;&c_a;&c_sh;&c_t;&c_o;&c_w;&c_n;&c_y;
        <ind>&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;</ind></trd>
      <trd lng="de">Edelstein</trd>
      <trd lng="fr">gemme</trd>
      <trd lng="hu">&eacute;kk&odblac;</trd>
      <trd lng="nl">edelsteen</trd>
      <trdgrp lng="pl">
        <trd>kamie&nacute; szlachetny</trd>,
        <trd>kamie&nacute; drogocenny</trd>
      </trdgrp>
      <trd
      lng="ru">&c_d;&c_r;&c_a;&c_g;&c_o;&c_c;&c_je;&c_n;&c_n;&c_y;&c_j;
        &c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;</trd>
      <trd lng="sv">&auml;delsten</trd>
    </drv>

    <drv mrk="sxton.lazur0o">
      <kap>lazur<tld/>o</kap>
      <snc mrk="sxton.lazur0o.MIN">
        <uzo tip="fak">MIN</uzo>
        <dif>
          Speco de <ref tip="super"
          cel="sxton.juvel0o">juvel&scirc;tono</ref> <ref
            tip="vid" cel="lazur.0o">lazur</ref>kolora:
          <ekz>
            kiel lazur<tld/>o estu mia vivo altvalora en viaj okuloj
            <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 11a, pluaj
            insidoj</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="fr">lapis lazuli</trd>
      <trdgrp lng="pl">
        <trd>lazuryt</trd>,
        <trd>lapis-lazuli</trd>
      </trdgrp>
      <trdgrp lng="pt">
        <trd>lazurita</trd>,
        <trd>l&aacute;pis-laz&uacute;li</trd>
      </trdgrp>
    </drv>

    <drv mrk="sxton.mejlo0o">
      <kap>mejlo<tld/>o</kap>
      <snc mrk="sxton.mejlo0o.TRA">
        <uzo tip="fak">TRA</uzo>
        <dif>
          Iu el la star&scirc;tonoj metitaj la&ubreve; mejla a&ubreve;
          alia distanco la&ubreve;
          landvojo.
        </dif>
        <trdgrp lng="pl">
          <trd>kamie&nacute; milowy</trd>,
          <trd>kamie&nacute; przydro&zdot;ny</trd>,
          <trd>znacznik drogowy</trd>
        </trdgrp>
      </snc>
      <snc mrk="sxton.mejlo0o.FIG">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Grava atingo en ankora&ubreve; ne finita afero:
          <ekz>
            la nova ekumena pre&gcirc;- kaj kantlibro <klr>...</klr>
            estas
            mejlo<tld/>o survoje al la unueco de la kristanoj
            <fnt>
              <aut>U. Matthias</aut>,
              <vrk>Recenzo pri &leftquot;Adoru&rightquot;</vrk>,
              <lok>soc.culture.esperanto, 2001-07-02</lok>
            </fnt>.
          </ekz>
        </dif>
        <trd lng="pl">kamie&nacute; milowy</trd>
      </snc>
      <trdgrp lng="fr">
        <trd>borne<klr tip="amb"> (kilom&eacute;trique)</klr></trd>,
        <trd>&eacute;tape importante</trd>
      </trdgrp>
    </drv>

    <drv mrk="sxton.tombo0o">
      <kap>tombo<tld/>o, <var><kap>tomb<tld/>o</kap></var></kap>
      <snc mrk="sxton.tombo0o.KOMUNE">
        <dif>
          <tld lit="&Scirc;"/>o, per kiu oni kovras a&ubreve; prisignas
          tombon:
          <ekz>
            tombo<tld/>oj kun krucoj kaj antikvaj &ccirc;izliteroj
            <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 3a, p.
            21a</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="fr">pierre tombale</trd>
      <trdgrp lng="pl">
        <trd>nagrobek</trd>,
        <trd>p&lstroke;yta nagrobna</trd>
      </trdgrp>
    </drv>

    <drv mrk="sxton.0hakisto">
      <kap><tld/>hakisto, <var><kap><tld/>ministo</kap></var></kap>
      <snc>
        <dif>
          Homo, kies metio estas tiri <tld/>ojn el <tld/>ejo:
          <ekz>
            oni dungis <tld/>hakistojn kaj &ccirc;arpentistojn, por
            renovigi
            la domon de la Eternulo
            <fnt><bib>MT</bib>, <lok>&Kr2; 24:12</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd
      lng="be">&c_k;&c_a;&c_m;&c_je;&c_n;&c_ja;&c_ch;&c_o;&c_s;</trd>
      <trdgrp lng="hu">
        <trd>k&odblac;fejt&odblac; (munk&aacute;s)</trd>,
        <trd>k&odblac;t&ouml;r&odblac;</trd>,
        <trd>k&odblac;b&aacute;ny&aacute;sz</trd>
      </trdgrp>
      <trd lng="pl">kamieniarz</trd>
      <trd
      lng="ru">&c_k;&c_a;&c_m;&c_je;&c_n;&c_o;&c_t;&c_jo;&c_s;</trd>
    </drv>

    <drv mrk="sxton.0trancxisto">
      <kap><tld/>tran&ccirc;isto</kap>
      <snc>
        <dif>
          Metiisto, kiu formtran&ccirc;as <tld/>ojn por konstruo.
        </dif>
      </snc>
      <trd
      lng="be">&c_k;&c_a;&c_m;&c_je;&c_n;&c_ja;&c_r;&c_e;&c_z;</trd>
      <trd lng="fr">tailleur de pierre</trd>
      <trd lng="hu">k&odblac;farag&oacute;</trd>
      <trd lng="nl">steenhouwer</trd>
      <trd lng="pl">kamieniarz</trd>
      <trd lng="sv">stenhuggare</trd>
    </drv>

    <drv mrk="sxton.gal0amalsano">
      <kap>gal<tld/>a malsano
        <fnt>
          <vrk><url
          ref="http://vortareto.free.fr/dictionnaire/esp-fr/m.htm">
          Dictionnaire m&eacute;dical
          &eacute;speranto-fran&ccedil;ais</url></vrk>
        </fnt></kap>
      <snc>
        <dif>
          <ref tip="sub" cel="san.mal0o">Malsano</ref> ka&ubreve;zata de
          <ref cel="sxton.0o.MED"><tld/>o<sncref/></ref> en la
          <ref cel="gal.0veziko">galveziko</ref> kaj (a&ubreve;) ties
          duktoj.
        </dif>
      </snc>
      <trd lng="en">cholelithiasis</trd>
      <trdgrp lng="fr">
        <trd>lithiase biliaire</trd>, <trd>chol&eacute;lithiase</trd>
      </trdgrp>
      <trd lng="pl">kamica &zdot;&oacute;&lstroke;ciowa</trd>
      <trd
      lng="ru">&c_zh;&c_je;&c_l;&c_ch;&c_n;&c_o;&c_k;&c_a;&c_m;&c_je;&c_n;&c_n;&c_a;&c_ja;
      &c_b;&c_o;&c_l;&c_je;&c_z;&c_n;&c_mol;</trd>
    </drv>

