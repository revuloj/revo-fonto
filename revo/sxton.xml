  <trd lng="pl">kamieniowa&cacute;</trd>
  <trd lng="pt">lapidar<klr tip="ind"> (matar)</klr></trd>
  <trd lng="ru">&c_p;&c_o;&c_b;&c_i;&c_v;&c_a;&c_t;&c_mol;
  &c_k;&c_a;&c_m;&c_n;&c_ja;&c_m;&c_i;</trd>
  <trd lng="sv">stena<klr> (till d&ouml;ds)</klr></trd>
</drv>

<drv mrk="sxton.sen0igi">
  <kap>sen<tld/>igi</kap>
  <snc mrk="sxton.sen0igi.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Senigi je <tld/>oj, ekzemple kampon por faciligi kultivon:
      <ekz>
        vinber&gcirc;ardenon havis mia amiko <klr>[&#x2026;]</klr> li
        &ccirc;irka&ubreve;fosis &gcirc;in,
        sen<tld/>igis &gcirc;in, kaj plantis en &gcirc;i plej
        bonspecajn vinberbran&ccirc;ojn
        <fnt><bib>MT</bib>, <lok>&Jes; 5:1-2</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">to clear of stones</trd>
  <trdgrp lng="de">
    <trd>entsteinen</trd>,
    <trd>von <ind>Steinen</ind> bereinigen</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>desempedrar</trd>,
    <trd>despedregar</trd>
  </trdgrp>
  <trd lng="fr">&eacute;pierrer</trd>
  <trd lng="it">dissodare</trd>
  <trd lng="nl">van <ind>steen</ind> ontdoen</trd>
  <trdgrp lng="pl">
    <trd>okdamieniowywa&cacute;</trd>,
    <trd>odgruzowywa&cacute;</trd>
  </trdgrp>
</drv>

<drv mrk="sxton.aer0o">
  <kap>aer<tld/>o</kap>
  <snc mrk="sxton.aer0o.aerolito">
    <uzo tip="fak">MET</uzo>
    <ref tip="dif" cel="aeroli.0o">aerolito</ref>
  </snc>
  <trd lng="br">aerolitenn</trd>
  <trdgrp lng="de">
    <trd>Meteorstein</trd>,
    <trd>Meteor</trd>,
    <trd>Aerolith</trd>,
    <trd>Sternschnuppe</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>meteor</trd>,
    <trd>aerolite</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>meteorito</trd>,
    <trd>aerolito</trd>
  </trdgrp>
  <trd lng="fr">a&eacute;rolithe</trd>
  <trd lng="it">meteorite</trd>
  <trd lng="nl">a&euml;roliet</trd>
  <trdgrp lng="pl">
    <trd>aerolit</trd>,
    <trd>meteoryt</trd>,
    <trd>meteor</trd>
  </trdgrp>
  <trd lng="pt">aer&oacute;lito</trd>
  <trd lng="sk">meteorit</trd>
  <trdgrp lng="ru">
