<drv mrk="sxton.0a">
  <kap><tld/>a</kap>
  <snc mrk="sxton.0a.MIN">
    <dif>
      El <tld/>o:
      <ekz>
        mi dezirus lo&gcirc;i en granda <tld/>a kastelo
        <fnt><bib>ElektFab</bib>, <lok>fi&scirc;kaptisto kaj lia
        edzino</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="en">stone</trd>
    <trdgrp lng="de">
      <trd>steinern</trd>,
      <trd>aus Stein</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>de piedra</trd>,
      <trd>p&eacute;treo</trd>
    </trdgrp>
    <trd lng="pl">kamienny</trd>
  </snc>
  <snc mrk="sxton.0a.FIG_fortika">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fortika nevenkebla:
      <ekz>
        ili dormis <tld/>an dormon<fnt>B</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fer.0a.FIG">fera <sncref/></ref>,
      <ref cel="sxtal.0a.FIG">&scirc;tala <sncref/></ref>
    </refgrp>
    <trdgrp lng="en">
      <trd>iron</trd>,
      <trd>cast-iron</trd>
    </trdgrp>
    <trd lng="de">wie ein Stein</trd>
    <trd lng="es">de piedra</trd>
    <trd lng="pl">kamienny</trd>
  </snc>
  <snc mrk="sxton.0a.FIG_senindulga">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ne mildigebla, senkompata, senindulga, senama:
      <ekz>
        koro <tld/>a<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="marmor">marmora</ref>
    </refgrp>
    <trdgrp lng="en">
      <trd>hard</trd>,
      <trd>stone</trd>
    </trdgrp>
    <trd lng="de">aus Stein</trd>
    <trd lng="es">de piedra</trd>
    <trd lng="pl">kamienny</trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_je;&c_n;&c_n;&c_y;</trd>
  <trdgrp lng="es">
    <trd>l&iacute;tico</trd>,
    <trd>de <ind>piedra</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>lithique</trd>,
    <trd>de <ind>pierre</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&odblac;-</trd>,
    <trd>k&odblac;kem&eacute;ny</trd>,
    <trd>k&odblac;b&odblac;l val&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>stenen</trd>,
    <trd>van <ind>steen</ind></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>litico</trd>,
    <trd>di <ind>pietra</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  <trd lng="tp">kiwen</trd>
</drv>

<drv mrk="sxton.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="sxton.0ego.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Granda nemovebla <tld/>o:
      <ekz>
        tombo <klr>[&#x2026;]</klr> elhakita el <tld/>ego
        <fnt><bib>NT</bib>, <lok>&Mar; 15:46</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="rok.0o">roko</ref>
    </refgrp>
  </snc>
  <trd lng="en">boulder</trd>
  <trdgrp lng="be">
    <trd>&c_u;&c_c;&c_jo;&c_s;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_l;&c_a;</trd>,
    <trd>&c_v;&c_a;&c_l;&c_u;&c_n;</trd>,
    <trd>&c_k;&c_a;&c_m;&c_l;&c_y;&c_g;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Fels</trd>
  <trd lng="es">roca</trd>
  <trd lng="fr">roc</trd>
  <trd lng="hu">k&odblac;szikla</trd>
  <trd lng="it">masso</trd>
  <trd lng="nl">rotsblok</trd>
  <trdgrp lng="pl">
    <trd>g&lstroke;az</trd>,
    <trd>ska&lstroke;a</trd>,
    <trd>kamol<klr> (pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_t;&c_jo;&c_s;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_l;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="sxton.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="sxton.0eto.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Tre malgranda <tld/>o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        lia bu&scirc;o estas plena de <tld/>etoj<fnt>Z</fnt>
        <klr>(li malklare prononcas)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gruz.0o">gruzo</ref>,
      <ref cel="sabl.0o">sablo</ref>
    </refgrp>
  </snc>
  <trd lng="en">pebble</trd>
  <trd lng="be">&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;&c_ch;&c_y;&c_k;</trd>
  <trdgrp lng="de">
    <trd>Steinchen</trd>,
    <trd>Kiesel</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>guijarro</trd>,
    <trd>chino</trd>,
    <trd>canto rodado</trd>
  </trdgrp>
  <trd lng="fr">caillou</trd>
  <trd lng="hu">kavics</trd>
  <trd lng="id">kerikil</trd>
  <trdgrp lng="it">
    <trd>pietruzza</trd>,
    <trd>sassolino</trd>
  </trdgrp>
  <trd lng="nl">kiezelsteen</trd>
  <trdgrp lng="pl">
    <trd>kamyk</trd>,
    <trd>kamyczek</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_sh;&c_je;&c_k;</trd>
</drv>

<drv mrk="sxton.0ejo">
  <kap><tld/>ejo, <var><kap><tld/>rompejo</kap></var>,
  <var><kap><tld/>minejo</kap></var></kap>
  <snc>
    <dif>
      Loko kie oni ekspluatas <tld/>ojn:
      <ekz>
        mi memoros viajn vortojn, dum mi rompos rokojn en
        <tld/>minejo
        <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;apitro
        30a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>stone quarry</trd>,
    <trd>stone mine</trd>
  </trdgrp>
  <trd lng="be">&c_k;&c_a;&c_m;&c_je;&c_n;&c_ja;&c_l;&c_o;&c_m;&c_n;&c_ja;</trd>
  <trd lng="de">Steinbruch</trd>
  <trd lng="es">cantera de piedra</trd>
  <trd lng="fr">carri&egrave;re de pierre</trd>
  <trdgrp lng="hu">
    <trd>k&odblac;t&ouml;r&odblac;</trd>,
    <trd>k&odblac;fejt&odblac;</trd>,
    <trd>k&odblac;b&aacute;nya</trd>
  </trdgrp>
  <trd lng="it">cava<klr> di pietra</klr></trd>
  <trd lng="nl">steengroeve</trd>
  <trdgrp lng="pl">
    <trd>kamienio&lstroke;om</trd>,
    <trd>wyrobisko <ind>kamieni</ind></trd>
  </trdgrp>
  <trd lng="pt">pedreira</trd>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_n;&c_o;&c_l;&c_o;&c_m;&c_n;&c_ja;</trd>
  <trd lng="sv">stenbrott</trd>
</drv>

<drv mrk="sxton.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="sxton.0igi.MIT">
  <uzo tip="fak">MIT</uzo>
    <dif>
      Transformi en &scirc;tonon, masive a&ubreve; supra&jcirc;e:
      <ekz>
      la sor&ccirc;istino <klr>[...]</klr> tu&scirc;is per la vergo anka&ubreve; la re&gcirc;on
      kaj &scirc;tonigis lin
      <fnt>
        <aut>Fratoj Grimm, trad. Kabe</aut>,
        <vrk>Elektitaj Fabeloj; Du Fratoj</vrk>
      </fnt>
      </ekz>.
    </dif>
  </snc>
  <snc mrk="sxton.0igi.FIG">
  <uzo tip="stl">FIG</uzo>
    <dif>
      Haltigi, senaktivigi:
      <ekz>
        Aleksandro, &scirc;tonigita de mirego
        <fnt>
          <aut>Henri Vallienne</aut>,
          <vrk>&Ccirc;u li?</vrk>,
          <lok>&ccirc;apitro dua</lok>
        </fnt>
      </ekz>.
    </dif>
  </snc>
  <trd lng="fr">p&eacute;trifier</trd>
</drv>

<drv mrk="sxton.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="sxton.0igxi.MIN">
    <dif>
      I&gcirc;i <tld/>a, simila al <tld/>o.
    </dif>
    <trdgrp lng="en">
      <trd>to become petrified</trd>,
      <trd>to petrify</trd>
    </trdgrp>
    <trd lng="de">versteinern</trd>
  <trd lng="de">erstarren</trd>
  <trd lng="es"><ind>fosilizar</ind>se</trd>
    <trdgrp lng="hu">
      <trd>elk&odblac;vesedik</trd>,
      <trd>k&odblac;v&eacute; v&aacute;lik;</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>skamienie&cacute;</trd>,
      <trd>obraca&cacute; si&eogonek; w
      <ind>kamie&nacute;</ind></trd>,
      <trd>zmienia&cacute; si&eogonek; w
      <ind>kamie&nacute;</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="sxton.0igxi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Rigidi&gcirc;i, senmovigi, i&gcirc;i neprogrespova:
      <ekz>
        &scirc;ia viza&gcirc;o <klr>[&#x2026;]</klr> aspektis kiel
        <tld/>i&gcirc;inta
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        mi preska&ubreve; <tld/>i&gcirc;is &ccirc;e la ekvido de
        leono
        <fnt><bib>Munchhausen</bib>, <lok>&ccirc;apitro
        1a</lok></fnt>;
      </ekz>
      <ekz>
        vivanta lingvo ne <tld/>i&gcirc;as;
      </ekz>
      <ekz>
        homo kun <tld/>i&gcirc;intaj ideoj.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>to become petrified</trd>,
      <trd>to turn to stone</trd>, <trd>to become inert</trd>
    </trdgrp>
    <trd lng="es">quedarse de piedra</trd>
    <trd lng="hu">k&odblac;v&eacute; dermed</trd>
    <trd lng="pl">skamienie&cacute;</trd>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_a;&c_m;&c_ja;&c_n;&c_je;&c_c;&c_mol;</trd>
  <trd lng="es"><ind>fosilizar</ind>se</trd>
  <trd lng="fr">se <ind>p&eacute;trifier</ind></trd>
  <trd lng="id">mem<ind>batu</ind></trd>
  <trdgrp lng="it">
    <trd>pietrificarsi</trd>,
    <trd>pietrificare<klr> (intr.)</klr></trd>
  </trdgrp>
  <trd lng="nl">verstenen</trd>
  <trd lng="ru">&c_o;&c_k;&c_a;&c_m;&c_je;&c_n;&c_je;&c_t;&c_mol;</trd>
  <trd lng="sv">f&ouml;rstenas</trd>
</drv>

<drv mrk="sxton.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxton.0umi.sxtonmortigi">
    <ref tip="dif" cel="mort.sxton0igi.KOMUNE"><tld/>mortigi</ref>
  </snc>
  <trdgrp lng="en">
    <trd>to stone</trd>,
    <trd>to throw stones at</trd>
  </trdgrp>
  <trd lng="be">&c_z;&c_a;&c_b;&c_ib;&c_c;&c_mol;
  <ind>&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;&c_n;&c_ja;&c_m;&c_ib;</ind>
  <klr>(&c_d;&c_a;
  &c_s;&c_mol;&c_m;&c_je;&c_r;&c_c;&c_ib;)</klr></trd>
  <trd lng="de">steinigen</trd>
  <trd lng="es">lapidar</trd>
