<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;ton</rad>/o
</kap>
<drv mrk="sxton.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Malmola pli malpli granda minerala maso:
      <ekz>
        ruli <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        peza estas <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi faros Jeruzalem amaso da <tld/>oj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        alligi <tld/>on al &jcirc;etilo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        sa&gcirc;o estas pli bona ol multekostaj <tld/>oj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj alportas sian <tld/>on <klr>(f: kunlaboras)</klr> por la
        konstruo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        trafi du celojn per unu <tld/>o
        <klr>(rimedo)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        per unu <tld/>o oni du &jcirc;etojn ne faras
        <klr>(por sukcesi oni ne devas samtempe celi plurajn atingota&jcirc;ojn.
        <ref tip="vid" cel="lepor.0o">leporo</ref>)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ne mi &jcirc;etis la unuan <tld/>on
        <klr>(f: ne mi unue atakis, iniciatis la malamikajn agojn)</klr>;
      </ekz>
      <ekz>
        &jcirc;eti <tld/>ojn sur la vojon de iu
        <klr>(f: intence aran&gcirc;i malfacila&jcirc;ojn al iu)</klr>;
      </ekz>
      <ekz>
        <ind><tld/>o de falpu&scirc;i&gcirc;o</ind>
        <klr>(falilo; f: ka&ubreve;zo de skandalo)</klr>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol; &c_p;&c_r;&c_je;&c_t;&c_k;&c_n;&c_o;&c_v;&c_je;&c_n;&c_i;&c_ja;</trd>
      </ekz>
      <ekz>
        sen<tld/>igi vinber&gcirc;ardenon<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Stein</trd>
  <trd lng="en">stone</trd>
  <trd lng="nl">steen</trd>
  <trd lng="pt">pedra</trd>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;</trd>
  <trd lng="sv">sten</trd>
</drv>

<drv mrk="sxton.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      El <tld/>o.
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fortika nevenkebla:
      <ekz>
        ili dormis <tld/>an dormon<fnt>B</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fer.0a">fera</ref>,
      <ref cel="sxtal.0a">&scirc;tala</ref>.
    </refgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ne mildigebla, senkompata, senindulga, senama:
      <ekz>
        koro <tld/>a<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="marmor">marmora</ref>.
    </refgrp>
  </snc>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="sxton.0ego">
  <kap><tld/>ego</kap>
  <snc>
    <dif>
      Granda nemovebla <tld/>o
    </dif>
    <refgrp tip="sin">
      <ref cel="rok.0o">roko</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Fels</trd>
  <trd lng="nl">rots</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_t;&c_jo;&c_s;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_l;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="sxton.0eto">
  <kap><tld/>eto</kap>
  <snc>
    <dif>
      Tre malgranda <tld/>o:
      <ekz>
        lia bu&scirc;o estas plena de <tld/>etoj<fnt>Z</fnt>
        <klr>(f: li malklare prononcas)</klr>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="gruz.0o">gruzo</ref>,
      <ref cel="silik.0o">siliko</ref>,
      <ref cel="sabl.0o">sablo</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">kiezelsteen</trd>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_sh;&c_je;&c_k;</trd>
</drv>

<drv mrk="sxton.0ejo0rompejo0minejo">
  <kap><tld/>ejo, <tld/>rompejo, <tld/>minejo</kap>
  <snc>
    <dif>
      Loko kie oni ekspluatas <tld/>ojn.
    </dif>
  </snc>
  <trd lng="de">Steinbruch</trd>
  <trd lng="nl">steengroeve</trd>
  <trd lng="pt">pedreira</trd>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_n;&c_o;&c_l;&c_o;&c_m;&c_n;&c_ja;</trd>
  <trd lng="sv">stenbrott</trd>
</drv>

<drv mrk="sxton.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      I&gcirc;i <tld/>a, simila al <tld/>o.
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Rigidi&gcirc;i, senmovigi, i&gcirc;i neprogrespova:
      <ekz>
        <tld/>i&gcirc;i pro mirego;
      </ekz>
      <ekz>
        &scirc;ia viza&gcirc;o aspektis kiel <tld/>i&gcirc;inta<fnt>Z</fnt>;
      </ekz>
      <ekz>
        vivanta lingvo ne <tld/>i&gcirc;as;
      </ekz>
      <ekz>
        homo kun <tld/>i&gcirc;intaj ideoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">verstenen</trd>
  <trd lng="ru">&c_o;&c_k;&c_a;&c_m;&c_je;&c_n;&c_je;&c_t;&c_mol;</trd>
  <trd lng="sv">f&ouml;rstenas</trd>
</drv>

<drv mrk="sxton.0hakisto0ministo">
  <kap><tld/>hakisto, <tld/>ministo</kap>
  <snc>
    <dif>
      Homo, kies metio estas tiri <tld/>ojn el <tld/>ejo.
    </dif>
  </snc>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_n;&c_o;&c_t;&c_jo;&c_s;</trd>
</drv>

<drv mrk="sxton.0trancxisto">
  <kap><tld/>tran&ccirc;isto</kap>
  <snc>
    <dif>
      Metiisto, kiu formtran&ccirc;as <tld/>ojn por konstruo.
    </dif>
  </snc>
  <trd lng="nl">steenhouwer</trd>
  <trd lng="sv">stenhuggare</trd>
</drv>

<drv mrk="sxton.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Pri&jcirc;eti iun per <tld/>o, <tld/>mortigi.
    </dif>
  </snc>
  <trd lng="de">steinigen</trd>
  <trd lng="nl">stenigen</trd>
  <trd lng="ru">&c_p;&c_o;&c_b;&c_i;&c_v;&c_a;&c_t;&c_mol; &c_k;&c_a;&c_m;&c_n;&c_ja;&c_m;&c_i;</trd>
  <trd lng="sv">stena<klr> (till d&ouml;ds)</klr></trd>
</drv>

<drv mrk="sxton.juvel0o">
  <kap>juvel<tld/>o</kap>
  <snc>
    <dif>
      Multekosta <tld/>o uzata por fari juvelojn
    </dif>
    <refgrp tip="sin">
      <ref cel="gem.0o">gemo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Edelstein</trd>
  <trd lng="nl">edelsteen</trd>
  <trd lng="ru">&c_d;&c_r;&c_a;&c_g;&c_o;&c_c;&c_je;&c_n;&c_n;&c_y;&c_j; &c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;</trd>
  <trd lng="sv">&auml;delsten</trd>
</drv>
</art>
