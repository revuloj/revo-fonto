        iri sur la <tld/>ejo de la pasinteco por kolekti rememorojn.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Stoppel</trd>
  <trd lng="en">stubble</trd>
  <trd lng="nl">stoppel</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_n;&c_i;&c_v;&c_mol;&c_jo;</trd>,
    <trd>&c_s;&c_t;&c_je;&c_r;&c_n;&c_ja;</trd>
