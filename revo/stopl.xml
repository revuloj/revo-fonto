<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stopl.xml,v 1.19 2023/09/04 15:14:32 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>stopl</rad>/o
</kap>
<drv mrk="stopl.0o">
  <kap><tld/>o</kap>
  <snc mrk="stopl.0o.AGR">
    <dif>
      Stumpigita <ref tip="super" cel="trunk.0eto">trunketo</ref> de greno restanta post la fal&ccirc;ado:
      <ekz>
        la nudaj sekaj <tld/>oj elstaris el la tero
        <fnt>
          <bib>Fab1</bib>
          <vrk>Elinjo-fingreto</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        juna civitano kun sova&gcirc;a <tld/>obarbo
        <fnt>
          <bib>MkM</bib>
          <lok>27</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pajl.0ero">pajlero</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_r;&c_zh;&c_y;&c_sh;&c_ch;&c_a;</trd>, 
    <trd>&c_ib;&c_r;&c_zh;&c_e;&c_w;&c_n;&c_ib;&c_k;</trd> 
  </trdgrp>
  <trdgrp lng="cs">
    <trd>strn</trd>,
    <trd>strnisko</trd>
  </trdgrp>
  <trd lng="de">Stoppel</trd>
  <trd lng="en">stubble</trd>
  <trd lng="es">estopa</trd>
  <trd lng="fr">chaume</trd>
  <trd lng="hu">torzs</trd>
  <trdgrp lng="ja">
    <trd>&#x5207;&#x682A;  [<ind>&#x304D;&#x308A;&#x304B;&#x3076;</ind>]</trd>,
    <trd>&#x5208;&#x682A;  [<ind>&#x304B;&#x304B;&#x3076;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">stoppel</trd>  
  <trd lng="pt">restolho</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_n;&c_i;&c_v;&c_mol;&c_jo;</trd>,
    <trd>&c_s;&c_t;&c_je;&c_r;&c_n;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">strnisko</trd>
  <trd lng="uk">&c_s;&c_t;&c_je;&c_r;&c_n;&c_ja;</trd>
</drv>

<drv mrk="stopl.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref cel="raz.0i">Razita</ref> lasinte mallongajn stumpojn:
      <ekz>
        lia ru&gcirc;a viza&gcirc;o kun helaj, <tld/>aj lipharoj brilis de bonfarto
        <fnt>
          <bib>HsT</bib>
          <lok>Sia propra mastro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a vango
        <fnt>
          <bib>HsT</bib>
          <lok>Sia propra mastro</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="hirt.0a">hirta</ref>
  </snc>
  <trd lng="de">stoppelig</trd>
</drv>

<drv mrk="stopl.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="stopl.0ejo.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      <ref tip="super" cel="kamp.0o.AGR">Kampo</ref>, sur kiu restas <tld/>oj:
      <ekz>
        la Falko flug&jcirc;etis sin sur la kolombojn sinforgese
        grajnumantajn sur la <tld/>ejo
        <fnt>
          <aut>J. Dorosmai, trad. J. Dorosmai jun. kaj J. Horvath</aut>
          <vrk>Fabloj kaj Aforismoj</vrk>
          <lok>2002</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ejoj kun &ccirc;evaloj kaj plugantoj
        <fnt>
          <bib>Tor</bib>
          <lok>V.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sur la <tld/>ejo de la pasinteco mi ekiras
        por kolekti postlasitajn grenerojn
        <fnt>
          <aut>J. Baghy</aut>
          <vrk>La koro de parenco</vrk>
          <lok>en: Dancu marionetoj!, 1927</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Stoppelfeld</trd>
  <trdgrp lng="fr">
    <trd>chaume<klr> (champ)</klr></trd>,
    <trd>&eacute;teule<klr> (champ)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5207;&#x682A;&#x7551;  [<ind>&#x304D;&#x308A;&#x304B;&#x3076;&#x306F;&#x305F;&#x3051;</ind>]</trd>,
    <trd>&#x5208;&#x7530;  [<ind>&#x304B;&#x308A;&#x305F;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_je;&c_r;&c_n;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_je;&c_r;&c_n;&c_i;&c_shch;&c_je;</trd>
  </trdgrp>
</drv>
</art>
