<vortaro>
  <art mrk="$Id: restitu.xml,v 1.4 2025/04/05 06:43:43 afido Exp $">
    <kap>
      <rad>restitu</rad>/i
      <fnt><bib>PVEF</bib></fnt>
    </kap>

<drv mrk="restitu.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="don.re0i">Redoni</ref> ion, kiu maljuste estis prenita:
      <ekz>
        mi postulas <tld/>on de miaj normalaj persona&jcirc;oj
        <fnt>
          <bib>MkM</bib>
          <lok>21</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>zur&uuml;ckerstatten</trd>,
      <trd>restituieren</trd>
    </trdgrp>
    <trd lng="en">restitute</trd>
    <trd lng="fr">restituer</trd>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>redoni</ke>.
  </rim>

<trd lng="be">&c_v;&c_ja;&c_r;&c_t;&c_a;&c_c;&c_mol; &c_u; &c_p;&c_a;&c_r;&c_a;&c_d;&c_k;&c_u; &c_r;&c_e;&c_s;&c_t;&c_y;&c_t;&c_u;&c_c;&c_y;&c_ib;</trd>
</drv>

  </art>

