    <kap>
      <rad>restitu</rad>/i
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="restitu.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="don.re0i">Redoni</ref> ion, kiu maljuste estis prenita:
      <ekz>
        mi postulas <tld/>on de miaj normalaj persona&jcirc;oj
        <fnt>
          <bib>MkM</bib>
          <lok>21</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>zur&uuml;ckerstatten</trd>,
      <trd>restituieren</trd>
    </trdgrp>
    <trd lng="en">restitute</trd>
    <trd lng="fr">restituer</trd>
  </snc>
</drv>

  </art>

