<art mrk="$Id: notic.xml,v 1.5 2015/07/04 08:37:40 revo Exp $">
<kap>
  <rad>notic</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="notic.0o">
  <kap><tld/>o</kap>
  <snc mrk="notic.0o.KOMUNE">
    <dif>
      Mallonga informa teksto ofte farita la&ubreve; &scirc;ablono:
      <ekz>
        la retejo &Gcirc;angalo proponas senpage la legadon de <tld/>oj 
        <klr>...</klr> per po&scirc;telefono
        <fnt>
          <aut>L. Domm&eacute;e</aut>,
          <lok><url ref="">La Amikeco, 2006-10-13, numero
          14a</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la vikipedio ne estas la &gcirc;usta loko por listigi <klr>...</klr> 
        biografiajn <tld/>ojn
        <fnt>
          <aut>V. Markov</aut>
          <vrk><url
          ref="http://eo.wikipedia.org/wiki/Vikipediista_diskuto:Chuck_SMITH">Vikipediista
          diskuto...</url></vrk>
          <lok>2005-06-26</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="depesx.0o">depe&scirc;o</ref>,
      <ref cel="not.0o">noto</ref>
    </refgrp>
  </snc>
  <trd lng="ca">not&iacute;cia <klr>(breu)</klr></trd>
  <trdgrp lng="cs">
    <trd>stručné sdělení</trd>,
    <trd>trochu delší poznámka</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>noticia <klr>(breve)</klr></trd>
    <trd>rese&ntilde;a</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>br&egrave;ve <klr> (journalistique)</klr></trd>,
    <trd>notice</trd>
  </trdgrp>
  <trd lng="it">notizia</trd>
  <trd lng="sk">poznámka</trd>
</drv>
</art>

