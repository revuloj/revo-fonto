<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Jid</rad>/o
</kap>
<drv mrk="jid.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">LIN</uzo>
  <snc>
    <dif>
      Lingvo uzata de oriente&ubreve;ropaj <ref tip="vid" cel="jud.0o">judoj</ref>.
      <tld lit="J"/>o estas &gcirc;ermana (germandevena),
      tamen enhavas multajn <ref tip="vid" cel="hebre.0o">hebreajn</ref>
      kaj <ref tip="vid" cel="slav.0a">slavajn</ref> vortojn.
      &Gcirc;ia skribo uzas hebrean alfabeton.
    </dif>
    <ref tip="vid" cel="hebre.0alingvo">hebrea lingvo</ref>
  </snc>
  <trd lng="en">Yiddish</trd>
  <trd lng="ru">&c_i;&c_d;&c_i;&c_sh;</trd>
</drv>
</art>

