<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rasp.xml,v 1.24 2023/10/20 19:20:11 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>rasp</rad>/i
</kap>
<drv mrk="rasp.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="skrap.0i.grati">Skrapi</ref> per multdenteta ilo 
      substancon ne tre malmolan, por &gcirc;in dispecetigi, pulvorigi a&ubreve; flokigi:
      <ekz>
        <tld/>i muskaton, froma&gcirc;on, karoton;
      </ekz>
      <ekz>
        mia blua libreto ri&ccirc;i&gcirc;is je recepto pri celeria sa&ubreve;co <klr>[&#x2026;]</klr> mia malbela
        manskribo instrukciis kiel el radiko el<tld/>i bongusta&jcirc;on
        <fnt>
          <aut>Spomenka &Scaron;timec</aut>
          <vrk>Ombro sur interna pejza&gcirc;o</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <ind>kokos<tld/>a&jcirc;oj</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2013/010973.php">Bombo-bankroto</url></vrk>
          <lok>2013</lok>
        </fnt>;
        <trd lng="de">Kokosrapseln</trd>
      </ekz>
      <ekz>
        grasigu la internon de la ladskatolo per iom da butero kaj sur&scirc;utu la
        <ind>pan<tld/>a&jcirc;o</ind>n 
        <klr>(vd <ref cel="pan.0ero">panero</ref>, <ref cel="pan.0erumi">panerumi</ref>)</klr>
        <fnt>
          <vrk><url ref="https://eo.wikibooks.org/wiki/Kuirlibro:Poffert">Kuirlibro:Poffert</url></vrk>
          <lok>Vikilibroj</lok>
        </fnt>;
        <trdgrp lng="de">
          <trd>Br&ouml;sel</trd>,
          <trd>Semmelmehl</trd>,
          <trd>Panniermehl</trd>,
        </trdgrp>
        <trd lng="hu">keny&eacute;rmorzsa</trd>
      </ekz>
      <ekz>
        lia barbo <tld/>is min dum kisoj
        <fnt>
          <aut>Spomenka &Scaron;timec</aut>
          <vrk>Ombro sur interna pejza&gcirc;o</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &#x2018;kion vi volas?&#x2019; li <tld/>is
        <klr>(diris <ref tip="super" cel="rauxk.0a">ra&ubreve;ke</ref>)</klr>, 
        altigante kandelon por nin ekzameni
        <fnt>
          <bib>KrM</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="fajl.0i.malkrudigi">fajli</ref>,
       <ref cel="muel.0i.AGR">mueli</ref>,
       <ref cel="rabot.0i">raboti</ref>,
       <ref cel="skrap.0i.grati">skrapi</ref>,
       <ref cel="sxlif.0i">&scirc;lifi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_je;&c_r;&c_c;&c_ib; &c_n;&c_a;
    <ind>&c_t;&c_a;&c_r;&c_c;&c_y;</ind></trd>,
    <trd>&c_a;&c_p;&c_r;&c_a;&c_c;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;
    <ind>&c_r;&c_a;&c_sh;&c_p;&c_ib;&c_l;&c_je;&c_m;</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nastrouhat</trd>,
    <trd>ra&scaron;plovat</trd>,
    <trd>strouhat</trd>
  </trdgrp>
  <trd lng="de">raspeln</trd>
  <trdgrp lng="en"><trd>to grate</trd>, <trd>rasp</trd></trdgrp>
  <trd lng="es">rallar</trd>
  <trdgrp lng="fr">
    <trd>raboter</trd>,
    <trd>r&acirc;per</trd>
  </trdgrp>
  <trd lng="hu">reszel</trd>
  <trdgrp lng="ja">
    <trd>&#x304A;&#x308D;&#x3057;&#x91D1;&#x3067;&#x304A;&#x308D;&#x3059;  <pr>&#x304A;&#x308D;&#x3057;&#x304B;&#x306D;&#x3067;&#x304A;&#x308D;&#x3059;</pr></trd>,
    <trd>&#x3059;&#x308A;&#x304A;&#x308D;&#x3059;</trd>,
    <trd>&#x3084;&#x3059;&#x308A;&#x3067;&#x524A;&#x308B;  <pr>&#x3084;&#x3059;&#x308A;&#x3067;&#x3051;&#x305A;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">raspen</trd>
  <trdgrp lng="pl">
    <trd>pi&lstroke;owa&cacute; <ind>tarnikiem</ind></trd>,
    <trd>raszplowa&cacute;</trd>,
    <trd>trze&cacute; na tarce <klr>(kulin.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_je;&c_r;&c_je;&c_t;&c_mol; &c_n;&c_a;
    &c_t;&c_jo;&c_r;&c_k;&c_je;</trd>,
    <trd>&c_o;&c_b;&c_r;&c_a;&c_b;&c_a;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;
    <ind>&c_r;&c_a;&c_sh;&c_p;&c_i;&c_l;&c_je;&c_m;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ra&scaron;p&lcaron;ova&tcaron;</trd>,
    <trd>str&uacute;ha&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x442;&#x435;&#x440;&#x442;&#x438; &#x43D;&#x430; &#x442;&#x435;&#x440;&#x442;&#x446;&#x456;</trd>,
    <trd>&#x43E;&#x431;&#x440;&#x43E;&#x431;&#x43B;&#x44F;&#x442;&#x438; &#x440;&#x430;&#x448;&#x43F;&#x456;&#x43B;&#x435;&#x43C;</trd>
  </trdgrp>
</drv>

<drv mrk="rasp.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Diversforma ilo, esence konsistanta el metala plato kun
      tran&ccirc;aj rondaj elstara&jcirc;oj:
      <ekz>
        <tld/>ilo de muskato
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e apuda stando oni vendis novforman specon de <tld/>ilo por erigi panon,
        froma&gcirc;on ktp.
        <fnt>
          <bib>KrM</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fajl.0ilo">fajlilo</ref>,
      <ref cel="rabot.0ilo">rabotilo</ref>,
      <ref cel="sxlif.0ilo">&scirc;lifilo</ref>
    </refgrp>
    <bld lok="&WCU;/thumb/7/7e/Denti_raspa_e_lima.jpg/640px-Denti_raspa_e_lima.jpg" prm="PD">surfaco de <tld/>ilo (maldekstre) kaj fajlilo (dekstre)
        <fnt>
          <aut>Tiesse, propra verko</aut>
          <lok><url ref="&WCW;/File:Denti_raspa_e_lima.jpg">&WCW;/File:Denti_raspa_e_lima.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_sh;&c_p;&c_ib;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ra&scaron;ple</trd>,
    <trd>struhadlo</trd>
  </trdgrp>
  <trd lng="de">Raspel</trd>
  <trdgrp lng="en">
    <trd>grater</trd>,
    <trd>file</trd>, <trd>rasp</trd>
  </trdgrp>
  <trd lng="es">rallador</trd>
  <trdgrp lng="fr">
    <trd>rabot</trd>,
    <trd>r&acirc;pe</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>reszel&odblac;</trd>,
    <trd>r&aacute;spoly</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x9B3C;&#x76EE;&#x3084;&#x3059;&#x308A;  <pr>&#x304A;&#x306B;&#x3081;&#x3084;&#x3059;&#x308A;</pr></trd>,
    <trd>&#x304A;&#x308D;&#x3057;&#x91D1;  <pr>&#x304A;&#x308D;&#x3057;&#x304C;&#x306D;</pr></trd>
  </trdgrp>
  <trd lng="nl">rasp</trd>
  <trdgrp lng="pl">
    <trd>tarnik</trd>,
    <trd>raszpla</trd>,
    <trd>trocinnik</trd>,
    <trd>tarka</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_jo;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_sh;&c_p;&c_i;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ra&scaron;p&lcaron;a</trd>,
    <trd>str&uacute;hadlo</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x442;&#x435;&#x440;&#x442;&#x43A;&#x430;</trd>,
    <trd>&#x440;&#x430;&#x448;&#x43F;&#x456;&#x43B;&#x44C;</trd>,
    <trd>&#x43D;&#x430;&#x43F;&#x438;&#x43B;&#x43E;&#x43A;</trd>
  </trdgrp>
</drv>

<drv mrk="rasp.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Kiu <tld/>as, kiu estas tre <ref tip="super" cel="glat.mal0a">malglata</ref>, 
      grata:
      <ekz>
        la vosta na&gcirc;ilo estas malrekta kun <tld/>a ostoplato
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/%C4%88ina_acipensero">&Ccirc;ina acipensero</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ni devos iri anka&ubreve; &gcirc;is la <tld/>a parto de la le&gcirc;oj
        <fnt>
          <bib>Rsp</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la kanto de la Blankgor&gcirc;a silvio estas rapida kaj <tld/>a 
        <klr>(<ref cel="rauxk.0a">ra&ubreve;ka</ref>)</klr>, kun plendema tono
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Blankgor%C4%9Da_silvio">Blankgor&gcirc;a silvio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a <klr>(nefluanta kaj nepolurita)</klr> stilo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aspr.0a.sento">aspra<sncref/></ref>,
      <ref cel="krud.0a.neprilaborita">kruda<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_u;&c_r;&c_p;&c_a;&c_t;&c_y;</trd>,
    <trd>&c_k;&c_a;&c_s;&c_t;&c_r;&c_u;&c_b;&c_a;&c_v;&c_a;&c_t;&c_y;</trd>,
    <trd>&c_zh;&c_o;&c_r;&c_s;&c_t;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>drsn&yacute;</trd>,
    <trd>s drsn&yacute;m povrchem</trd>
  </trdgrp>
  <trd lng="de">rau</trd>
  <trdgrp lng="en">
    <trd>raspy</trd>,
    <trd>grating</trd>, <trd>scratchy</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>raboteux</trd>,
    <trd>r&acirc;peux</trd>,
    <trd>rugueux</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;rdes</trd>,
    <trd>reszel&odblac;s</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3056;&#x3089;&#x3056;&#x3089;&#x3057;&#x305F;</trd>,
    <trd>&#x304D;&#x3081;&#x306E;&#x7C97;&#x3044;  <pr>&#x304D;&#x3081;&#x306E;&#x3042;&#x3089;&#x3044;</pr></trd>,
    <trd>&#x3057;&#x308F;&#x304C;&#x308C;&#x305F;</trd>,
    <trd>&#x3054;&#x3064;&#x3054;&#x3064;&#x3057;&#x305F;</trd>
  </trdgrp>
  <trd lng="nl">ruw</trd>
  <trd lng="pl">szorstki</trd>
  <trd lng="ru">&c_sh;&c_je;&c_r;&c_sh;&c_a;&c_v;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>drap&lcaron;av&yacute;</trd>,
    <trd>drsn&yacute;</trd>
  </trdgrp>
</drv>
</art>
