<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rasp.xml,v 1.18 2018/08/26 18:10:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>rasp</rad>/i
</kap>
<drv mrk="rasp.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Skrapi per speciala ilo substancon ne tre malmolan por &gcirc;in
      dispecetigi, pulvorigi a&ubreve; flokigi:
      <ekz>
        <tld/>i sukeron, froma&gcirc;on, karoton;
      </ekz>
      <ekz>
        <ind>pan<tld/>ita&jcirc;o</ind>.
        <trd lng="hu">keny&eacute;rmorzsa</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_je;&c_r;&c_c;&c_ib; &c_n;&c_a;
    <ind>&c_t;&c_a;&c_r;&c_c;&c_y;</ind></trd>,
    <trd>&c_a;&c_p;&c_r;&c_a;&c_c;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;
    <ind>&c_r;&c_a;&c_sh;&c_p;&c_ib;&c_l;&c_je;&c_m;</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nastrouhat</trd>,
    <trd>ra&scaron;plovat</trd>,
    <trd>strouhat</trd>
  </trdgrp>
  <trd lng="de">raspeln</trd>
  <trdgrp lng="en"><trd>to grate</trd>, <trd>rasp</trd></trdgrp>
  <trd lng="es">rallar</trd>
  <trdgrp lng="fr">
    <trd>raboter</trd>,
    <trd>r&acirc;per</trd>
  </trdgrp>
  <trd lng="hu">reszel</trd>
  <trd lng="nl">raspen</trd>
  <trdgrp lng="pl">
    <trd>pi&lstroke;owa&cacute; <ind>tarnikiem</ind></trd>,
    <trd>raszplowa&cacute;</trd>,
    <trd>trze&cacute; na tarce <klr>(kulin.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_je;&c_r;&c_je;&c_t;&c_mol; &c_n;&c_a;
    &c_t;&c_jo;&c_r;&c_k;&c_je;</trd>,
    <trd>&c_o;&c_b;&c_r;&c_a;&c_b;&c_a;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;
    <ind>&c_r;&c_a;&c_sh;&c_p;&c_i;&c_l;&c_je;&c_m;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ra&scaron;p&lcaron;ova&tcaron;</trd>,
    <trd>str&uacute;ha&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="rasp.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Diversforma ilo, esence konsistanta el metala plato kun
      tran&ccirc;aj
      rondaj elstara&jcirc;oj.
    </dif>
    <refgrp tip="sin">
      <ref cel="fajl.0ilo">fajlilo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_sh;&c_p;&c_ib;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ra&scaron;ple</trd>,
    <trd>struhadlo</trd>
  </trdgrp>
  <trd lng="de">Raspel</trd>
  <trdgrp lng="en">
    <trd>grater</trd>,
    <trd>file</trd>, <trd>rasp</trd>
  </trdgrp>
  <trd lng="es">rallador</trd>
  <trdgrp lng="fr">
    <trd>rabot</trd>,
    <trd>r&acirc;pe</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>reszel&odblac;</trd>,
    <trd>r&aacute;spoly</trd>
  </trdgrp>
  <trd lng="nl">rasp</trd>
  <trdgrp lng="pl">
    <trd>tarnik</trd>,
    <trd>raszpla</trd>,
    <trd>trocinnik</trd>,
    <trd>tarka</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_jo;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_sh;&c_p;&c_i;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ra&scaron;p&lcaron;a</trd>,
    <trd>str&uacute;hadlo</trd>
  </trdgrp>
</drv>

<drv mrk="rasp.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Kiu raspas a&ubreve; malglatas, gratas iom, kiam oni tu&scirc;as:
      <ekz>
        <tld/>a tabulo, ha&ubreve;to;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a vo&ccirc;o
        (<ref tip="vid" cel="rauxk.0a">ra&ubreve;ka</ref>);
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a <klr>(nefluanta kaj nepolurita)</klr> stilo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_u;&c_r;&c_p;&c_a;&c_t;&c_y;</trd>,
    <trd>&c_k;&c_a;&c_s;&c_t;&c_r;&c_u;&c_b;&c_a;&c_v;&c_a;&c_t;&c_y;</trd>,
    <trd>&c_zh;&c_o;&c_r;&c_s;&c_t;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>drsn&yacute;</trd>,
    <trd>s drsn&yacute;m povrchem</trd>
  </trdgrp>
  <trd lng="de">rau</trd>
  <trdgrp lng="en">
    <trd>raspy</trd>,
    <trd>grating</trd>, <trd>scratchy</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>raboteux</trd>,
    <trd>r&acirc;peux</trd>,
    <trd>rugueux</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;rdes</trd>,
    <trd>reszel&odblac;s</trd>
  </trdgrp>
  <trd lng="nl">ruw</trd>
  <trd lng="pl">szorstki</trd>
  <trd lng="ru">&c_sh;&c_je;&c_r;&c_sh;&c_a;&c_v;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>drap&lcaron;av&yacute;</trd>,
    <trd>drsn&yacute;</trd>
  </trdgrp>
</drv>
</art>
