<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: larik.xml,v 1.25 2019/06/30 18:21:03 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>larik</rad>/o
</kap>
<drv mrk="larik.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Arbo el <ref tip="lst" cel="konifer.0o"
      lst="voko:koniferoj">koniferoj</ref> kun unujaraj pingloformaj
      folietoj
      kaj malgrandaj pli malpli ovoformaj strobiloj;
      <ref tip="lst" cel="genr.0o"
      lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="pinac.0oj">pinacoj</ref>
      (<trd lng="la">Larix</trd>):
      <ekz>
        sube sur la deklivoj kreskas abioj kaj foliarboj, pli supre <tld/>oj, piceoj kaj
        cembroj
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2005/008395.php"
              >Vizito al la nacia parko Vanoise</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        61&#8239;% de la ligno venas el piceoj, malpli gravas fago, pino, <tld/>o kaj abio
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2013/010952.php"
              >Lige al ligno</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/7/70/Illustration_Larix_decudua0.jpg/373px-Illustration_Larix_decudua0.jpg"
      prm="PD"
      >botanika tabulo pri Kutima <tld/>o (Larix decidua)
        <fnt>
          <aut>Prof. Dr. Otto Wilhelm Thom&eacute;: &leftquot;Flora von Deutschland, &Ouml;sterreich und der Schweiz&rightquot;, 1885, Gera, Germanujo</aut>
          <lok><url ref="&WCW;/File:Illustration_Larix_decudua0.jpg"
              >https://commons.wikimedia.org/wiki/File:Illustration_Larix_decudua0.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_s;&c_t;&c_o;&c_w;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="bg">&c_l;&c_i;&c_s;&c_t;&c_v;&c_je;&c_n;&c_i;&c_c;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>l&agrave;rix</trd>,
    <trd>aler&ccedil;</trd>
  </trdgrp>
  <trd lng="de">L&auml;rche</trd>
  <trd lng="en">larch</trd>
  <trd lng="fa">&a_l;&a_A;&a_r;&f_y;&f_k;&a_s;</trd>
  <trd lng="fr">m&eacute;l&egrave;ze</trd>  
  <trd lng="hu">v&ouml;r&ouml;sfeny&odblac;</trd>
  <trd lng="nl">lariks</trd>
  <trd lng="pl">modrzew</trd>
  <trdgrp lng="pt">
    <trd>l&aacute;rix</trd>,
    <trd>l&aacute;rice</trd>,    
    <trd>lar&iacute;cio</trd>,    
    <trd>lari&ccedil;o</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_i;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_i;&c_c;&c_a;</trd>
</drv>
</art>
