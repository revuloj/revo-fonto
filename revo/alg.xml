<vortaro>
<art mrk="$Id: alg.xml,v 1.40 2023/10/28 08:33:04 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>alg</rad>/oj <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="alg.0oj">
  <kap><tld/>oj</kap>
  <tezrad fak="BOT"/>
  <snc mrk="alg.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Speciori&ccirc;a grupo de unu- a&ubreve; plur&ccirc;elaj
      viva&jcirc;oj, plej ofte vivantaj en akvo:
      <ekz>
        tiu viro, glitinte sur tufo el malsekaj <tld/>oj, estis 
        rapidigita kun kapo malsupre sur akran rifon
        <fnt><aut>H. Vallienne</aut>, <vrk>&Ccirc;u 
        li?</vrk>, <lok>1908</lok></fnt>.
      </ekz>
    </dif>
    <rim>
      La <tld/>ojn oni nuntempe ordigas al la regno
      <ctl>Protoctistae</ctl> (= unue establi&gcirc;intoj),
      por kio ankora&ubreve; ne uzi&gcirc;is e-a vorto.
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_g;&c_a;&c_v;&c_ib;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_v;&c_o;&c_d;&c_a;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_v;&c_o;&c_d;&c_a;&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;</trd>
  </trdgrp>
  <trd lng="bg">&c_v;&c_o;&c_d;&c_o;&c_r;&c_a;&c_s;&c_l;&c_i;</trd>
  <trd lng="ca">algues</trd>
  <trd lng="cs">&rcaron;asy</trd>
  <trd lng="de">Alge</trd>
  <trd lng="en">algae</trd>
  <trd lng="es">algas</trd>
  <trd lng="fa">&a_j;&a_l;&a_b;&f_k;</trd>
  <trd lng="fr">algues</trd>
  <trd lng="he">&#x5D0;&#x5B7;&#x5E6;&#x5D5;&#x5B9;&#x5EA;</trd>
  <trdgrp lng="hu">
    <trd>moszatok</trd>, 
    <trd>alg&aacute;k</trd>
  </trdgrp>
  <trd lng="id">alga</trd>
  <trd lng="ja">&#x85FB;&#x985E;  <pr>&#x305D;&#x3046;&#x308B;&#x3044;</pr></trd>
  <trd lng="nl">algen</trd>
  <trd lng="pt">algas</trd>
  <trd lng="ru">&c_v;&c_o;&c_d;&c_o;&c_r;&c_o;&c_s;&c_l;&c_i;</trd>
  <trdgrp lng="sk">
    <trd>morsk&eacute; <ind>riasy</ind></trd>,
    <trd>riasy</trd>
  </trdgrp>
  <trd lng="sv">alger</trd>
  <trd lng="uk">&c_v;&#x43E;&c_d;&#x43E;&c_r;&#x43E;&c_s;&c_t;&c_ib;</trd>
</drv>

<drv mrk="alg.blu0o">
  <kap>blu<tld/>o, <var><kap>blua <tld/>o</kap></var></kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="alg.blu0o.BAK">
    <uzo tip="fak">BAK</uzo>
    <dif>
      Iu el <ref tip="dif" cel="bakter.ciano0oj.BAK">cianobakterioj</ref>
      (iam misklasifikitaj kiel algoj):
      <ekz>
        ekzistas esta&jcirc;oj, ekzemple la bluaj <tld/>oj <klr>...</klr>,
        kiuj 
        bone fartas en alta temperaturo (&gcirc;is 90 &deg;C)
        <fnt>
          <aut>M. B&eacute;la</aut>,
          <vrk><url ref="&STEB;biologio/enkonduko_en_la_teorian_biologion/teoria_biologio.htm">
          Enkonduko en la teorian biologion</url></vrk>,
          <lok>SUK, 1976</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd><ind>alga</ind> blava</trd>,
    <trd><ind>alga</ind> blauverda</trd>,
    <trd>esquizof&iacute;cia</trd>
  </trdgrp>
  <trd lng="fr">algue bleue</trd>
  <trd lng="id"><ind>alga</ind> biru</trd>
  <trdgrp lng="zh">
    <trd>&#x84DD;&#x85FB; <pr>l&#xE1;nz&#x1CE;o</pr></trd>,
    <trd>&#x85CD;&#x85FB; <pr>l&#xE1;nz&#x1CE;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="alg.brun0oj">
  <kap>brun<tld/>oj, <var><kap>brunaj <tld/>oj</kap></var></kap>
  <snc mrk="alg.brun0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <tld lit="A"/>oj precipe maraj, brunkoloraj
      pro klorofilo c kaj flava farbosubstanco, 
      konsistigantaj apartan filumon
      (<trd lng="la">Phaeophycophyta</trd>):
      <ekz>
        vakameo <klr>...</klr> estas <klr>...</klr> brun<tld/>o uzata 
        en homa nutrado, precipe en Japanio
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Vakameo">
          vakameo</url>, 2010-12-12</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x68D5;&#x85FB; <pr>z&#x14D;ngz&#x1CE;o</pr></trd>,
    <trd>&#x77F3;&#x82B1;&#x83DC; <pr>sh&#xED;hu&#x101;c&#xE0;i</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_b;&c_u;&c_r;&c_y;&c_ja; <ind>&c_v;&c_o;&c_d;&c_a;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ib;</ind></trd>
  <trdgrp lng="ca">
    <trd><ind>algues</ind> brunes</trd>,
    <trd>fe&ograve;fits</trd>
  </trdgrp>
  <trd lng="cs">hn&ecaron;d&eacute; &rcaron;asy</trd>
  <trd lng="de">Braunalgen</trd>
  <trd lng="en">brown algae</trd>
  <trd lng="fa">&a_j;&a_l;&a_b;&f_k;&zwnj;&a_h;&a_A;&f_y;
  &a_q;&a_h;&a_w;&a_h;&zwnj;&a_A;&f_y;</trd>
  <trd lng="fr">algues brunes</trd>
  <trd lng="hu">barnamoszatok</trd>
  <trd lng="id"><ind>alga</ind> cokelat</trd>
  <trd lng="ja">&#x8910;&#x85FB;&#x985E;  <pr>&#x304B;&#x3063;&#x305D;&#x3046;&#x308B;&#x3044;</pr></trd>
  <trd lng="nl">bruinalgen</trd>
  <trd lng="pt">algas marrons</trd>
  <trd lng="sk">hned&eacute; riasy</trd>
  <trd lng="uk">&c_b;&c_u;&c_r;&c_ib; &c_v;&#x43E;&c_d;&#x43E;&c_r;&#x43E;&c_s;&c_t;&c_ib;</trd>
</drv>

<drv mrk="alg.rugx0oj">
  <kap>ru&gcirc;<tld/>oj, <var><kap>ru&gcirc;aj <tld/>oj</kap></var></kap>
  <snc mrk="alg.rugx0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <tld lit="A"/>oj precipe maraj, ru&gcirc;koloraj
      pro klorofilo a kaj aliaj farbosubstancoj,
      konsistigantaj apartan filumon
      (<trd lng="la">Rhodophycota</trd>).
    </dif>
    <trdgrp lng="zh">
    <trd>&#x7D05;&#x85FB; <pr>h&#xF3;ngz&#x1CE;o</pr></trd>,
    <trd>&#x7EA2;&#x85FB; <pr>h&#xF3;ngz&#x1CE;o</pr></trd>,
    <trd>&#x7D2B;&#x83DC; <pr>z&#x1D0;c&#xE0;i</pr></trd>,
    <trd>&#x539F;&#x7D05;&#x85FB;&#x7DB1; <pr>yu&#xE1;nh&#xF3;ngz&#x1CE;og&#x101;ng</pr></trd>,
    <trd>&#x539F;&#x7EA2;&#x85FB;&#x7EB2; <pr>yu&#xE1;nh&#xF3;ngz&#x1CE;og&#x101;ng</pr></trd>,
    <trd>&#x6761;&#x6591;&#x7D2B;&#x83DC; <pr>ti&#xE1;ob&#x101;nz&#x1D0;c&#xE0;i</pr></trd>,
    <trd>&#x689D;&#x6591;&#x7D2B;&#x83DC; <pr>ti&#xE1;ob&#x101;nz&#x1D0;c&#xE0;i</pr></trd>,
    <trd>&#x4E4C;&#x83DC; <pr>w&#x16B;c&#xE0;i</pr></trd>,
    <trd>&#x70CF;&#x83DC; <pr>w&#x16B;c&#xE0;i</pr></trd>,
    <trd>&#x7D05;&#x6BDB;&#x83DC;&#x79D1; <pr>h&#xF3;ngm&#xE1;oc&#xE0;ik&#x113;</pr></trd>,
    <trd>&#x7EA2;&#x6BDB;&#x83DC;&#x79D1; <pr>h&#xF3;ngm&#xE1;oc&#xE0;ik&#x113;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_ch;&c_y;&c_r;&c_v;&c_o;&c_n;&c_y;&c_ja; <ind>&c_v;&c_o;&c_d;&c_a;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ib;</ind></trd>
  <trdgrp lng="ca">
    <trd><ind>algues</ind> vermelles</trd>,
    <trd>rod&ograve;fits</trd>
  </trdgrp>
  <trd lng="de">Rotalgen</trd>
  <trd lng="en">red algae</trd>
  <trd lng="fa">&a_j;&a_l;&a_b;&f_k;&zwnj;&a_h;&a_A;&f_y;
  &a_q;&a_r;&a_m;&a_z;</trd>
  <trd lng="fr">algues rouges</trd>
  <trd lng="hu">v&ouml;r&ouml;smoszatok</trd>
  <trd lng="id"><ind>alga</ind> merah</trd>
  <trd lng="ja">&#x7D05;&#x85FB;&#x985E;  <pr>&#x3053;&#x3046;&#x305D;&#x3046;&#x308B;&#x3044;</pr></trd>
  <trd lng="nl">roodalgen</trd>
  <trd lng="pt">algas vermelhas</trd>
</drv>

<drv mrk="alg.verd0oj">
  <kap>verd<tld/>oj, <var><kap>verdaj <tld/>oj</kap></var></kap>
  <snc mrk="alg.verd0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <tld lit="A"/>oj el diversaj grupoj, verdkoloraj
      pro klorofilo a kaj b:
      <ekz>
        ene de la unuopaj taksonoj de la verd<tld/>oj ekzistas diversaj 
        organizaj formoj, de la unu&ccirc;eluloj kaj &ccirc;elkolonioj &gcirc;is la 
        mult&ccirc;elaj taloj
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Verdalgoj">
          verdalgoj</url>, 2010-06-13</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x7DA0;&#x85FB;&#x9580; <pr>l&#xFC;z&#x1CE;om&#xE9;n</pr></trd>,
    <trd>&#x7EFF;&#x85FB;&#x95E8; <pr>l&#xFC;z&#x1CE;om&#xE9;n</pr></trd>,
    <trd>&#x7DA0;&#x85FB; <pr>l&#xFC;z&#x1CE;o</pr></trd>,
    <trd>&#x7EFF;&#x85FB; <pr>l&#xFC;z&#x1CE;o</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_z;&c_ja;&c_l;&c_jo;&c_n;&c_y;&c_ja; <ind>&c_v;&c_o;&c_d;&c_a;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ib;</ind></trd>
  <trdgrp lng="ca">
    <trd><ind>algues</ind> verdes</trd>,
    <trd>clor&ograve;fits</trd>
  </trdgrp>
  <trd lng="cs">zelen&eacute; &rcaron;asy</trd>
  <trd lng="de">Gr&uuml;nalgen</trd>
  <trd lng="en">green algae</trd>
  <trd lng="fa">&a_j;&a_l;&a_b;&f_k;&zwnj;&a_h;&a_A;&f_y;
  &a_s;&a_b;&a_z;</trd>
  <trd lng="fr">algues vertes</trd>
  <trd lng="hu">z&ouml;ldmoszatok</trd>
  <trd lng="id"><ind>alga</ind> hijau</trd>
  <trd lng="ja">&#x7DD1;&#x85FB;&#x985E;  <pr>&#x308A;&#x3087;&#x304F;&#x305D;&#x3046;&#x308B;&#x3044;</pr></trd>
  <trd lng="la">Chlorophycota</trd>
  <trd lng="nl">groenalgen</trd>
  <trd lng="pt">algas verdes</trd>
  <trd lng="sk">zelen&eacute; riasy</trd>
  <trd lng="uk">&c_z;&c_je;&#x43B;&c_je;&#x43D;&c_ib; &c_v;&#x43E;&c_d;&#x43E;&c_r;&#x43E;&c_s;&c_t;&c_ib;</trd>
</drv>
</art>
