<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>*</ofc>
    <rad>singult</rad>/i <fnt><bib>PIV</bib></fnt>
</kap>

<drv mrk="singult.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="singult.0i.SNC">
    <dif>
    Eligi bruon, ka&ubreve;zatan de spasma kuntiri&gcirc;o de la <ref tip="vid"
    cel="diafra.0o.ANA">diafragmo</ref>.
    </dif>
  </snc>
  <trd lng="fr">hoqueter</trd>
</drv>

<drv mrk="singult.0o">
  <kap><tld/>o</kap>
  <snc mrk="singult.0o.SNC">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <trd lng="de">Schluckauf</trd>
  <trd lng="en">hiccup</trd>
  <trdgrp lng="fr">
    <trd>hoquet</trd>,
    <trd><ind>myoclonie</ind> phr&eacute;noglottique</trd>
  </trdgrp>
</drv>
</art>

