<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: madras.xml,v 1.4 2024/01/25 20:10:33 afido Exp $">
    <kap>
      <rad>Madras</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="madras.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      Nomo de la urbo <ref tip="sin" cel="cxenaj.0o">&Ccirc;enajo</ref> &gcirc;is 1996:
      <ekz>
        la teozofoj transloki&gcirc;is unue al Bombajo, poste al <tld/>o
        <fnt>
          <bib>Be</bib>
          <aut>Zofia Banet-Fornalowa</aut>
          <vrk><url ref="https://books.google.de/books?id=o2c_8gmwexQC&amp;pg=PA70">Janusz Korczak kaj la Oriento</url></vrk>
          <lok>Beletetra Almanako 5, junio 2009, p. 70</lok>
        </fnt>;
      </ekz>
      <ekz>
        la flugtempo inter <tld/>o kaj Delhio estas 150 minutoj, de Mumbajo (iama
        Bombajo) estas 70 minutoj kaj de &Ccirc;enajo (iama Madraso) estas 40 minutoj
        <fnt>
          <vrk><url ref="https://www.oocities.org/bharato/eventoj/ak2008/UnuaBulteno-AziaKongreso2008.pdf">La 5-a Azia Kongreso de Esperanto okazos en</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <rim>
        Cetere islama lernejo nomi&gcirc;as
      <ref tip="hom" cel="medrese.0o">madraso a&ubreve; medreseo</ref>.
    </rim>
    <trd lng="de">Madras</trd>
    <trd lng="en">Madras</trd>
    <trd lng="fr">Madras</trd>
    <trdgrp lng="ta">
      <trd>&#xBAE;&#xBC6;&#xB9F;&#xBCD;&#xBB0;&#xBBE;&#xBB8;&#xBCD;</trd>,
      <trd>&#xBAE;&#xBA4;&#xBB0;&#xBBE;&#xBB8;&#xBCD;</trd>
    </trdgrp>
  </snc>
  <trd lng="ja">&#x30DE;&#x30C9;&#x30E9;&#x30B9;</trd>
  <trdgrp lng="zh">
    <trd>&#x99AC;&#x5FB7;&#x62C9;&#x65AF; <pr>M&#x1CE;d&#xE9;l&#x101;s&#x12B;</pr></trd>,
    <trd>&#x9A6C;&#x5FB7;&#x62C9;&#x65AF; <pr>M&#x1CE;d&#xE9;l&#x101;s&#x12B;</pr></trd>
  </trdgrp>
</drv>

  </art>

