<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>surogat</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="suroga.0o">
  <kap><tld/>o</kap>
  <snc mrk="suroga.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="anstat.0ilo">Anstata&ubreve;ilo</ref>, produkto
      kiun
      oni povas uzi anstata&ubreve; iu alia, &ccirc;ar &gcirc;i posedas
      proksimume samajn ecojn, sed &gcirc;i estas malpli kosta, a&ubreve; malpli
      bonkvalita:
      <ekz>
        glano <klr>...</klr> uzatas kiel <tld/>o por kafo
        <klr>...</klr>, kaj tiucele oni sekigas &gcirc;in en specialaj
        dometoj
        <fnt>
          <aut>J. Kozarac, trad. D. Klobu&ccaron;ar</aut>,
          <vrk><url
          ref="http://public.carnet.hr/esperanto-kel/slavonia_arbaro.pdf">
          Slavonia Arbaro</url></vrk>,
          <lok>Kroata Esperanto-Ligo, 2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_r;&c_a;&c_g;&c_a;&c_t;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_ja;&c_n;&c_ja;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Surrogat</trd>,
    <trd>Ersatzstoff</trd>,
    <trd>Notbehelf</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ersatz</trd>,
    <trd>substitut</trd>,
    <trd>succ&eacute;dan&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>p&oacute;tl&eacute;k</trd>,
    <trd>p&oacute;tszer</trd>,
    <trd>p&oacute;tanyag</trd>,
    <trd>szurrog&aacute;tum</trd>
  </trdgrp>
  <trd lng="it">surrogato</trd>
  <trd lng="nl">surrogaat</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_r;&c_r;&c_o;&c_g;&c_a;&c_t;</trd>,
    <trd>&c_e;&c_r;&c_z;&c_a;&c_c;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_je;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
