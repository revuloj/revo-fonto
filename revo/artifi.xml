<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>artifik</rad>/o
</kap>
<drv mrk="artifi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Arte lerta farmaniero.
      <ekz>
        <tld/>o de &jcirc;onglisto.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="akroba.0ajxo">akrobata&jcirc;o</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Arte  lerta kombina&jcirc;o:
      <ekz>
        <tld/>o de advokato.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="turn">elturni&gcirc;o</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Trompa ruza&jcirc;o:
      <ekz>
        <tld/>o de  fikomercisto.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="intrig.0o">intrigo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Trick</trd>,
    <trd>Kunstgriff</trd>,
    <trd>Kniff</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>artifice</trd>,
    <trd>truc</trd>,
    <trd>astuce</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&#1090;&#1088;&#1102;&#1082;</trd>,
    <trd>&#1091;&#1083;&#1086;&#1074;&#1082;&#1072;</trd>,
    <trd>&#1091;&#1093;&#1080;&#1097;&#1088;&#1077;&#1085;&#1080;&#1077;</trd>,
    <trd>&#1087;&#1088;&#1080;&#1105;&#1084;</trd>
  </trdgrp>
</drv>

<drv mrk="artifi.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld/>e  fari.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>confectionner astucieusement</trd>,
    <trd>fabriquer (une histoire)</trd>
  </trdgrp>
</drv>

<drv mrk="artifi.sen0a">
  <kap>sen<tld/>a</kap>
  <snc>
    <dif>
      Sincera, lojala, honesta.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>naturel</trd>,
    <trd>franc</trd>
  </trdgrp>
  <trd
  lng="ru">&#1073;&#1077;&#1089;&#1093;&#1080;&#1090;&#1088;&#1086;&#1089;&#1090;&#1085;&#1099;&#1081;</trd>
</drv>
</art>
