    <trd>&c_v;&c_i;&#x43A;&c_r;&c_u;&c_t;</trd>,
    <trd>&#x43F;&c_r;&c_i;&c_j;&#x43E;&#x43C;</trd>,
    <trd>&c_h;&c_i;&c_t;&c_r;&#x43E;&c_shch;&c_ib;</trd>,
    <trd>&c_h;&c_i;&c_t;&c_r;&c_u;&c_v;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_v;&c_i;&#x43A;&c_r;&c_u;&c_t;&c_a;&c_s;</trd>,
    <trd>&c_t;&c_r;&#x44E;&#x43A;</trd>
  </trdgrp>
</drv>

<drv mrk="artifi.0a">
  <kap><tld/>a</kap>
  <snc mrk="artifi.0a.KOMUNE">
    <dif>
      Imprese lerta, arta, eble tiom, ke &gcirc;i erarigas:
      <ekz>
        &ccirc;iu parolas en <tld/>a delikateco, preska&ubreve; tute
        kiel nobeloj
        <fnt><bib>Revizoro</bib>, <lok>akto 2a, sceno 1a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ingxeni.0a">in&gcirc;enia</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_r;&c_y;&c_t;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>lstiv&yacute;</trd>,
    <trd>z&aacute;ludn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>artificiel</trd>,
    <trd>artificieux</trd>,
    <trd>astucieux</trd>,
    <trd>contourn&eacute;</trd>,
    <trd>mani&eacute;r&eacute;</trd>,
    <trd>sophistiqu&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>furfangos</trd>,
    <trd>fort&eacute;lyos</trd>,
    <trd>cseles</trd>,
    <trd>tr&uuml;kk&ouml;s</trd>,
    <trd>agyaf&uacute;rt</trd>
  </trdgrp>
  <trdgrp lng="it">
      <trd>artefatto<klr> (agg.)</klr></trd>,
      <trd>artificioso</trd>,
      <trd>arzigogolato</trd>
    </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7B56;&#x7565;&#x306B;&#x3088;&#x308B;  <pr>&#x3055;&#x304F;&#x308A;&#x3083;&#x304F;&#x306B;&#x3088;&#x308B;</pr></trd>,
    <trd>&#x3054;&#x307E;&#x304B;&#x3057;&#x306E;</trd>
  </trdgrp>
  <trd lng="pl">sprytny</trd>
  <trd lng="sk">&lcaron;stiv&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43F;&c_r;&c_i;&c_t;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43C;&c_a;&c_j;&c_s;&c_t;&c_je;&c_r;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43C;&c_i;&c_t;&c_je;&c_c;&#x44C;&#x43A;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="artifi.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="artifi.0i.KOMUNE">
    <dif>
      Fari lerta&jcirc;ojn impresajn kaj ne travideblajn por eksteruloj:
      <ekz>
        &gcirc;in &ccirc;ion la franco <tld/>as
        <fnt><bib>Revizoro</bib>, <lok>akto 1a, sceno 2a</lok></fnt>..
      </ekz>
      <ekz>
        vi estas bona kamarado, &scirc;i diris <klr>...</klr> Vi ne
        faras bruon,
        kaj vi ne <tld/>as
        <fnt><bib>InfanTorent2</bib>, <lok>dua parto, &ccirc;apitro
        1a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="afekt.0i">afekti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>oklamat</trd>,
    <trd>podv&aacute;d&ecaron;t</trd>,
    <trd>p&rcaron;elst&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>confectionner astucieusement</trd>,
    <trd>manigancer</trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5EA;&#x5D7;&#x5D1;&#x5DC;
  &#x5EA;&#x5B7;&#x5D7;&#x5B0;&#x5D1;&#x5BC;&#x5D5;&#x5BC;&#x5DC;&#x5B8;&#x5D4;</trd>
  <trdgrp lng="hu">
    <trd>mesterkedik</trd>,
    <trd>&uuml;gyeskedik</trd>,
    <trd>megj&aacute;tszik <klr tip="ind">(tettet)</klr></trd>,
    <trd>tettet</trd>,
    <trd>csellel el&eacute;r</trd>,
    <trd>furfanggal el&eacute;r</trd>
  </trdgrp>
  <trdgrp lng="it">
      <trd>truccare</trd>,
      <trd>architettare<klr> (fig.)</klr></trd>
    </trdgrp>
  <trd lng="ja">&#x3054;&#x307E;&#x304B;&#x3059;</trd>
  <trdgrp lng="pl">
      <trd>robi&cacute; <ind>sztuczk&eogonek;</ind></trd>,
      <trd>stosowa&cacute; <ind>fortel</ind></trd>,
      <trd>u&zdot;ywa&cacute; <ind>fortelu</ind></trd>,
      <trd>stosowa&cacute; <ind>kruczek</ind></trd>,
      <trd>stosowa&cacute; <ind>wybieg</ind></trd>
    </trdgrp>
  <trdgrp lng="sk">
    <trd>alebo pref&iacute;kane</trd>,
    <trd>robi&tcaron; nie&ccaron;o k&uacute;zeln&iacute;cky</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&#x43E;&c_b;&c_i;&c_t;&c_i; &#x43C;&c_a;&c_h;&c_ib;&#x43D;&c_a;&c_c;&c_ib;&c_ji;</trd>,
    <trd>&c_v;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F; &c_d;&#x43E; &c_v;&c_i;&#x43A;&c_r;&c_u;&c_t;&c_a;&c_s;&c_ib;&c_v;</trd>,
    <trd>&c_h;&c_i;&c_t;&c_r;&#x43E;&c_shch;&c_ib;&c_v;</trd>,
    <trd>&c_h;&c_i;&c_t;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&#x43B;&c_je;&c_s;&c_t;&c_i; &c_ib;&#x43D;&c_t;&c_r;&c_i;&c_gu;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="artifi.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="artifi.sen0a.KOMUNE">
    <dif>
      <ref tip="super" cel="honest.0a.verema">Honesta</ref>, ne uzanta
      malsimplajn a&ubreve; ka&scirc;ajn rimedojn:
      <ekz>
        maljunulo, gaja, simpla kaj sen<tld/>a
        <fnt><bib>Fab2</bib>, <lok>Monteto de elfoj</lok></fnt>.
      </ekz>
      <ekz>
        Marta <klr>...</klr> a&ubreve;skultis la sen<tld/>an rakontadon
        de la
        kudristino
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        la sen<tld/>a mieno de la knabino
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 17a, Adaha
        ser&ccirc;as
        Omaron</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_h;&c_ib;&c_t;&c_r;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bezelstn&yacute;</trd>,
    <trd>nel&iacute;&ccaron;en&yacute;</trd>,
    <trd>nez&aacute;ludn&yacute;</trd>,
    <trd>prost&yacute;</trd>,
    <trd>up&rcaron;&iacute;mn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>naturel</trd>,
    <trd>franc</trd>
  </trdgrp>
  <trd lng="hu">mesterk&eacute;letlen</trd>
  <trdgrp lng="it">
