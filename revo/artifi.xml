<drv mrk="artifi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="artifi.0o.KOMUNE">
    <dif>
      Arta farmaniero, kiu &scirc;ajnas simpla, sed en efektivo lertas,
      iluzias,
      e&ccirc; trompas.
      <ekz>
        <tld/>o de &jcirc;onglisto;
      </ekz>
      <ekz>
        se mi ne estus jam maljuna ulo, mi tuj irus en politeknikan
        lernejon kaj lernus la <tld/>ojn de la mondo
        <fnt><bib>Fab4</bib>, <lok>Pupludisto</lok></fnt>.
      </ekz>
      <ekz>
        Dio kreis la homon virtema; sed la homoj fordonis sin al multaj
        <tld/>oj
        <fnt><bib>MT</bib>, <lok>&Pre; 7:29</lok></fnt>.
      </ekz>
      <ekz>
        multaj pastroj <klr>...</klr> per lertaj <tld/>oj trompas la
        simplulojn
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 20a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akroba.0ajxo">akrobata&jcirc;o</ref>,
      <ref cel="turn.el0ajxo">elturna&jcirc;o</ref>,
      <ref cel="implik.0ajxo">implika&jcirc;o</ref>,
      <ref cel="intrig.0o">intrigo</ref>
    </refgrp>
    <trdgrp lng="en">
      <trd>trick</trd>,
      <trd>artifice</trd>,
      <trd>con game</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>trucco</trd>,
      <trd>artificio</trd>,
      <trd>espediente</trd>
    </trdgrp>
    <trd lng="pt"> artif&iacute;cio</trd>
    <trdgrp lng="pl">
      <trd>sztuczka</trd>,
      <trd>fortel</trd>,
      <trd>kruczek<klr> (spos&oacute;b)</klr></trd>,
      <trd>wybieg</trd>
    </trdgrp>
    <trdgrp lng="th">
      <trd>&#xE01;&#xE25;&#xE2D;&#xE38;&#xE1A;&#xE32;&#xE22;</trd>,
      <trd>&#xE40;&#xE25;&#xE48;&#xE2B;&#xE4C;&#xE40;&#xE2B;&#xE25;&#xE35;&#xE48;&#xE22;&#xE21;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x4F0E; <pr>j&#xEC;</pr></trd>,
    <trd>&#x4F0E;&#x4FE9; <pr>j&#xEC;li&#x1CE;ng</pr></trd>,
    <trd>&#x4F0E;&#x5006; <pr>j&#xEC;li&#x1CE;ng</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_u;&c_k;</trd>,
    <trd>&c_v;&c_y;&c_k;&c_r;&c_u;&c_t;</trd>,
    <trd>&c_h;&c_ib;&c_t;&c_r;&c_y;&c_k;</trd>,
    <trd>&c_h;&c_ib;&c_t;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>lest</trd>,
    <trd>pleticha</trd>,
    <trd>trik</trd>,
    <trd>&uacute;skok</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Trick</trd>,
    <trd>Kunstgriff</trd>,
    <trd>Kniff</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>artifice</trd>,
    <trd>astuce</trd>,
    <trd>truc</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&#x5EA;&#x5B7;&#x5D7;&#x5B0;&#x5D1;&#x5BC;&#x5D5;&#x5BC;&#x5DC;&#x5B8;&#x5D4;</trd>,
    <trd>&#x5EA;&#x5E2;&#x5DC;&#x5D5;&#x5DC;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fog&aacute;s</trd>,
    <trd>furfang</trd>,
    <trd>tr&uuml;kk</trd>,
    <trd>fort&eacute;ly</trd>,
    <trd>cselfog&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7B56;&#x7565;  <pr>&#x3055;&#x304F;&#x308A;&#x3083;&#x304F;</pr></trd>,
    <trd>&#x8853;&#x7B56;  <pr>&#x3058;&#x3085;&#x3063;&#x3055;&#x304F;</pr></trd>,
    <trd>&#x624B;&#x7DF4;&#x624B;&#x7BA1;  <pr>&#x3066;&#x308C;&#x3093;&#x3066;&#x304F;&#x3060;</pr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_ju;&c_k;</trd>,
    <trd>&c_u;&c_l;&c_o;&c_v;&c_k;&c_a;</trd>,
