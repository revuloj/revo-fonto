    <trd>&ccaron;estn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&#x435;&#x445;&#x438;&#x442;&#x440;&#x438;&#x439;</trd>,
    <trd>&#x43F;&#x440;&#x43E;&#x441;&#x442;&#x438;&#x439;</trd>,
