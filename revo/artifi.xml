<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>artifik</rad>/o
</kap>
<drv mrk="artifi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Arte lerta farmaniero.
      <ekz>
        <tld/>o de &jcirc;onglisto.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="akroba.0ajxo">akrobata&jcirc;o</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Arte lerta kombina&jcirc;o:
      <ekz>
        <tld/>o de advokato.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="turn">elturni&gcirc;o</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Trompa ruza&jcirc;o:
      <ekz>
        <tld/>o de fikomercisto.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="intrig.0o">intrigo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Trick</trd>,
    <trd>Kunstgriff</trd>,
    <trd>Kniff</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>artifice</trd>,
    <trd>truc</trd>,
    <trd>astuce</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_ju;&c_k;</trd>,
    <trd>&c_u;&c_l;&c_o;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_u;&c_h;&c_i;&c_shch;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_jo;&c_m;</trd>
  </trdgrp>
</drv>

<drv mrk="artifi.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="A"/>e fari.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>confectionner astucieusement</trd>,
    <trd>fabriquer (une histoire)</trd>
  </trdgrp>
</drv>

<drv mrk="artifi.sen0a">
  <kap>sen<tld/>a</kap>
  <snc>
    <dif>
      Sincera, lojala, honesta.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>naturel</trd>,
    <trd>franc</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_s;&c_h;&c_i;&c_t;&c_r;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
