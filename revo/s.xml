<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>*</ofc><rad>s</rad> <fnt><bib>PIV1</bib></fnt>
</kap>
<subart>
<drv mrk="s.0.konsonanto">
  <kap><tld/></kap>
    <uzo tip="fak">LIN</uzo>
    <uzo tip="fak">TIP</uzo>
    <uzo tip="fak">GRA</uzo>
    <uzo tip="fak">FON</uzo>
  <snc>    
    <dif>
      <ref tip="dif" cel="konson1.0o">Konsonanto</ref>, senvo&ccirc;a
      frikativa <ref tip="dif" cel="dental.0o">dentalo</ref> [s].
    </dif>
  <trd lng="fr">s<klr> (consonne)</klr></trd>  
  </snc>
  <snc>
     <dif>
      La dudekdua <ref tip="dif" cel="grafem.0o">grafemo</ref> de la
      Esperanta alfabeto.
     </dif>
  <trd lng="fr">s<klr> (lettre)</klr></trd>
  </snc>
 </drv>
</subart>

<subart>
<drv mrk="s.0.nomo">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">GRA</uzo>
    <uzo tip="fak">FON</uzo>    
    <dif>
      Nomo de tiuj <ref tip="dif" cel="fonem.0o">fonemo</ref> kaj
      grafemo.    
    </dif>
  <trd lng="fr">s</trd>
  </snc>
</drv>
</subart>
</art>

