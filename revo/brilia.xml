<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>briliant</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="brilia.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="brilia.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Facetita <ref tip="super" cel="diaman.0o.MIN">diamanto</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_y;&c_l;&c_mol;&c_ja;&c_n;&c_t;</trd>
  <trd lng="de">Brillant</trd>
  <trd lng="es">brillante</trd>
  <trd lng="fr">brillant <klr tip="ind">(diamant)</klr></trd>
  <trd lng="hu">brili&aacute;ns</trd>
  <trd lng="pl">brylant</trd>
  <trd lng="ru">&c_b;&c_r;&c_i;&c_l;&c_l;&c_i;&c_a;&c_n;&c_t;</trd>
</drv>
</art>
