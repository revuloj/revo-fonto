<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: brest.xml,v 1.4 2023/02/01 07:26:37 revo Exp $">
    <kap>
      <rad>Brest</rad>/o
    </kap>

<drv mrk="brest.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">POL</uzo><uzo tip="fak">GEOG</uzo>
      <dif>
      <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">Urbo</ref> en
      okcidenta
      <ref tip="malprt" cel="belorus.B0ujo">Belorusujo</ref>, proksime
      apud la pola landlimo:
      <ekz>
        unuigo de katolika kaj ortodoksa eklezioj, sur teritorio de
        Polujo kaj Litvo,
        proklamita en oktobro 1596 per eklezia koncilio en urbo <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=388328">Bresta religia unio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la sie&gcirc;o de <tld/>o okazis en 22a&#x202F;&ndash;&#x202F;29a de junio 1941
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=626899">Sie&gcirc;o de Bresto</url></lok>
        </fnt>;
      </ekz>

      <ekz>
        la <tld/>o-Litovska paco diktita de la monar&hcirc;ia Germanio
        <fnt>
          <aut>V. I. Lenin, trad. Jurij Finkel</aut>
          <vrk><url ref="https://www.marxists.org/esperanto/lenin/imperiismo.htm">Imperiismo kiel la
          plej alta stadio de
              kapitalismo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Brest</trd>
    <trd lng="fr">Brest</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_B;&c_je;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_je;</trd>,
    <trd>&c_B;&c_r;&c_e;&c_s;&c_t; <klr tip="amb">(&c_b;&c_je;&c_l;&c_a;&c_r;&c_u;&c_s;&c_k;&c_ib;)</klr></trd>
  </trdgrp>
  <trd lng="uk">&c_B;&c_r;&c_je;&c_s;&c_t;</trd>
</drv>

  </art>

