<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>alkal</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="alkal.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Nomo de kelkaj metalaj oksidoj kaj hidroksidoj kun samaj reakcioj
      (potaso, sodo, kc).
    </dif>
  </snc>
  <trdgrp lng="de">
   <trd>Alkali</trd>,
   <trd>Laugensalz</trd>
  </trdgrp>
  <trd lng="fr">alcali</trd>
  <trd lng="nl">alcali</trd>
  <trd lng="pt">&aacute;lcali</trd>
  <trd lng="ru">&c_shch;&c_jo;&c_l;&c_o;&c_ch;&c_mol;</trd>
</drv>
<drv mrk="alkal.0eca">
  <kap><tld/>(ec)a</kap>
  <snc>
    <dif>
      Havanta saman reakcion kiel <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>alkalisch</trd>,
    <trd>alkaliartig</trd>
  </trdgrp>
  <trd lng="fr">alcalin</trd>
  <trd lng="nl">alkalisch</trd>
  <trd lng="pt">alcalino</trd>
  <trd lng="ru">&c_shch;&c_je;&c_l;&c_o;&c_ch;&c_n;&c_o;&c_j;</trd>
</drv>
</art>
