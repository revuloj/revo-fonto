<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: alkal.xml,v 1.23 2022/03/11 14:25:46 revo Exp $">
<kap>
  <rad>alkal</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="alkal.0o">
  <kap><tld/>o</kap>
  <snc mrk="alkal.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Nomo de kelkaj metalaj oksidoj kaj hidroksidoj kun samaj reakcioj:
      <ref tip="vid" cel="potas.0o">potaso</ref>, 
      <ref tip="vid" cel="sod.0o">sodo</ref>
      kaj ceteraj:
      <ekz>
        en kemio, <tld/>o estas baza, jona salo de <tld/>a metalo a&ubreve; ter<tld/>a metalo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=224667">Alkalo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_ch;&c_o;&c_l;&c_a;&c_ch;</trd>
  <trd lng="ca">&agrave;lcali</trd>
  <trdgrp lng="cs">
    <trd>alk&aacute;lie</trd>,
    <trd>z&aacute;sada <klr>(chem.)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
   <trd>Alkali</trd>,
   <trd>Laugensalz</trd>
  </trdgrp>
  <trd lng="en">alkali</trd>
  <trd lng="fa">&a_q;&a_l;&f_y;&a_A;</trd>
  <trd lng="fr">alcali</trd>
  <trd lng="he">&#x5D0;&#x5B7;&#x5DC;&#x5E7;&#x5B8;&#x5DC;&#x5B4;&#x5D9;</trd>
  <trd lng="nl">alcali</trd>
  <trdgrp lng="pl">
    <trd>zasada</trd>,
    <trd>&lstroke;ug</trd>,
    <trd>alkalium</trd>
  </trdgrp>
  <trd lng="pt">&aacute;lcali</trd>
  <trd lng="ru">&c_shch;&c_jo;&c_l;&c_o;&c_ch;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>alk&aacute;lia</trd>,
    <trd>z&aacute;sada <klr>(chemick&aacute;)</klr></trd>,
    <trd>z&aacute;sada</trd>
  </trdgrp>
  <trd lng="uk">&#x43B;&#x443;&#x433;</trd>
</drv>

<drv mrk="alkal.0eca">
  <kap><tld/>eca, <var><kap><tld/>a</kap></var></kap>
  <snc mrk="alkal.0eca.KEM">
    <dif>
      Havanta saman reakcion kiel <tld/>o:
      <ekz>
        la <tld/>aj metaloj de la <tld/>a familio konsistas el la elementoj litio,
        natrio, kalio, rubidio, cezio, kaj franciumo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4234">Alkala metalo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Bogoria</nom> estas sala, <tld/>a lago kiu ku&scirc;as en vulkana regiono en duon-trafala
        baseno sude de Baringo, Kenjo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=163304">Bogoria</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_ch;&c_o;&c_l;&c_a;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">alcal&iacute;</trd>
  <trdgrp lng="cs">
    <trd>alkalick&yacute;</trd>,
    <trd>z&aacute;sadit&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>alkalisch</trd>,
    <trd>alkaliartig</trd>
  </trdgrp>
  <trd lng="en">alkaline</trd>
  <trd lng="es">alcalino</trd>
  <trd lng="fa">&a_q;&a_l;&f_y;&a_A;&f_y;&f_y;</trd>
  <trd lng="fr">alcalin</trd>
  <trd lng="he">&#x5D1;&#x5BC;&#x5B0;&#x5E1;&#x5B4;&#x5D9;&#x5E1;&#x5B4;&#x5D9;</trd>
  <trd lng="nl">alkalisch</trd>
  <trdgrp lng="pl">
    <trd>alkaliczny</trd>,
    <trd>zasadowy</trd>
  </trdgrp>
  <trd lng="pt">alcalino</trd>
  <trd lng="ru">&c_shch;&c_je;&c_l;&c_o;&c_ch;&c_n;&c_o;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>z&aacute;sadit&yacute;</trd>,
    <trd>z&aacute;sadit&yacute; roztok</trd>
  </trdgrp>
</drv>
</art>
