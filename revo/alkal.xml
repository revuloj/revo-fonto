<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>alkal</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="alkal.0o">
  <kap><tld/>o</kap>
  <snc mrk="alkal.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Nomo de kelkaj metalaj oksidoj kaj hidroksidoj kun samaj reakcioj:
      <ref tip="vid" cel="potas.0o">potaso</ref>, 
      <ref tip="vid" cel="sod.0o">sodo</ref>
      kaj ceteraj.
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_ch;&c_o;&c_l;&c_a;&c_ch;</trd>
  <trd lng="ca">&agrave;lcali</trd>
  <trdgrp lng="de">
   <trd>Alkali</trd>,
   <trd>Laugensalz</trd>
  </trdgrp>
  <trd lng="en">alkali</trd>
  <trd lng="fa">&a_q;&a_l;&f_y;&a_A;</trd>
  <trd lng="fr">alcali</trd>
  <trd lng="nl">alcali</trd>
  <trdgrp lng="pl">
    <trd>zasada</trd>,
    <trd>&lstroke;ug</trd>,
    <trd>alkalium</trd>
  </trdgrp>
  <trd lng="pt">&aacute;lcali</trd>
  <trd lng="ru">&c_shch;&c_jo;&c_l;&c_o;&c_ch;&c_mol;</trd>
  <trd lng="sk">z&aacute;sada <klr>(chemick&aacute;)</klr></trd>
</drv>

<drv mrk="alkal.0eca">
  <kap><tld/>eca, <var><kap><tld/>a</kap></var></kap>
  <snc mrk="alkal.0eca.KEM">
    <dif>
      Havanta saman reakcion kiel <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_ch;&c_o;&c_l;&c_a;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">alcal&iacute;</trd>
  <trdgrp lng="de">
    <trd>alkalisch</trd>,
    <trd>alkaliartig</trd>
  </trdgrp>
  <trd lng="en">alkaline</trd>
  <trd lng="es">alcalino</trd>
  <trd lng="fa">&a_q;&a_l;&f_y;&a_A;&f_y;&f_y;</trd>
  <trd lng="fr">alcalin</trd>
  <trd lng="nl">alkalisch</trd>
  <trdgrp lng="pl">
    <trd>alkaliczny</trd>,
    <trd>zasadowy</trd>
  </trdgrp>
  <trd lng="pt">alcalino</trd>
  <trd lng="ru">&c_shch;&c_je;&c_l;&c_o;&c_ch;&c_n;&c_o;&c_j;</trd>
  <trd lng="sk">z&aacute;sadit&yacute;</trd>
</drv>
</art>
