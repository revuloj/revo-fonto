<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>teleskop</rad>/o
</kap>
<drv mrk="telesk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      Optika instrumento por observi tre malproksimajn
      objektojn, precipe astrojn.
    </dif>
    <refgrp tip="super">
      <ref cel="lorn.0o">lorno</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Fernrohr</trd>,
    <trd>Teleskop</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>t&eacute;lescope</trd>,
    <trd>lunette astronomique</trd>
  </trdgrp>
  <trd lng="nl">telescoop</trd>
  <trd lng="ru">&c_t;&c_je;&c_l;&c_je;&c_s;&c_k;&c_o;&c_p;</trd>
</drv>
</art>
