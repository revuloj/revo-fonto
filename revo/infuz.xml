<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>infuz</rad>/i
</kap>
<drv mrk="infuz.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ver&scirc;i bolantan akvon a&ubreve; flua&jcirc;on sur ian substancon por ke tiu
      flua&jcirc;o prenu la esencajn sukojn el tiu substanco:
      <ekz><tld/>i teon, kamomilon, kafon.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="dekokt.0i">dekokti</ref>.
    </refgrp>
  </snc>
  <trd lng="en">infuse</trd>
  <trd lng="nl">
    een <ind>aftreksel</ind> maken door opgieting 
    van een vloeistof
  </trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_v;&c_a;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_a;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="infuz.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Solva&jcirc;o ricevita per <tld/>ado.  
    </dif>
  </snc>
  <trd lng="nl">aftreksel</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_v;&c_a;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_o;&c_j;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_o;&c_j;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
