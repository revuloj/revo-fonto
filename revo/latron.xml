<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: latron.xml,v 1.16 2024/01/15 16:14:08 revo Exp $">
<kap>
  <rad>latron</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="latron.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">POE</uzo>
  <snc mrk="latron.0o.rabisto">
    <dif>
     <ref tip="dif" cel="rab.0isto">Rabisto</ref>:
      <ekz>
        la <tld/>oj prirabis kaj murdis lin kaj poste lin enterigis en
        &ccirc;i loko tiel dezerta<fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro 23a,
        p. 165a</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi kuneksportis tian diskon a&ubreve; &ccirc;u ni &gcirc;in <tld/>u el lia fikamero
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 9</lok>
        </fnt>?
      </ekz>
      <ekz>
        i&gcirc;is &ccirc;ies diakono: de l' piulo, de l' <tld/>o
        <fnt>
          <vrk><url ref="https://hymnary.org/text/audu_sonas_pastoralo">A&ubreve;du: Sonas
          pastoralo</url></vrk>
          <lok>Hymnary.org</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_b;&c_o;&c_j;&c_n;&c_ib;&c_k;</trd>
  <trd lng="ca">lladre</trd>
  <trdgrp lng="cs">
    <trd>krimin&aacute;ln&iacute;k</trd>,
    <trd>lotr</trd>,
    <trd>zlod&ecaron;j</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Sch&auml;cher</trd>,
    <trd>Stehler</trd>,
    <trd>Brigant</trd>
  </trdgrp>
  <trd lng="es">ladr&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>brigand</trd>,
    <trd>larron</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>lator</trd>,
    <trd>&uacute;ton&aacute;ll&oacute;</trd>
  </trdgrp>
  <trd lng="nl">rover<klr tip="ind"> (zeldz.)</klr></trd>
  <trd lng="pl">z&lstroke;odziej</trd>
  <trdgrp lng="pt">
    <trd>ladr&atilde;o</trd>,
    <trd>salteador</trd>
  </trdgrp>
  <trd lng="ro">ho&tcomma;</trd>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_b;&c_o;&c_j;&c_n;&c_i;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>krimin&aacute;lnik</trd>,
    <trd>lotor</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_z;&c_b;&c_ib;&c_j;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_b;&c_ib;&c_zh;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_l;&c_o;&c_t;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5C71;&#x8CCA; <pr>sh&#x101;nz&#xE9;i</pr></trd>,
    <trd>&#x5C71;&#x8D3C; <pr>sh&#x101;nz&#xE9;i</pr></trd>
  </trdgrp>
</drv>
</art>

