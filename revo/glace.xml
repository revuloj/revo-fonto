<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap><rad>glace</rad>/a
      <fnt>Z</fnt></kap>

    <drv mrk="glace.0a">
      <kap><tld/>a</kap>
      <snc>
        <dif>
          (Pri surfaco): Briligita per glatigo a&ubreve;
          <ref cel="polur.0o">poluro</ref> (p.p. papero,
          ledo, ekrano ktp):
          <ekz>
            <ind><tld/>aj gantoj</ind>;
            <trd lng="hu">glasz&eacute;keszty&udblac;</trd>
            <trd lng="ru">&c_l;&c_a;&c_j;&c_k;&c_o;&c_v;&c_y;&c_je; &c_p;&c_je;&c_r;&c_ch;&c_a;&c_t;&c_k;&c_i;</trd>
          </ekz><ekz>
            en 1957 naski&gcirc;is <ctl>Nuntempa Bulgario</ctl>
            <klr>...</klr> kun konstanta aspekto: ri&ccirc;e ilustrita, sur
            <tld/>a papero <klr>...</klr>
	    <fnt>
	      <aut>G. Waringhien</aut>:
	      <vrk>Revuoj de l' novaj revuoj</vrk>.
              <lok>Nica Literatura Revuo, 3:6</lok>
	    </fnt>.
          </ekz>
        </dif>
      </snc>
  <trdgrp lng="be">
        <trd>&c_g;&c_l;&c_ja;&c_n;&c_c;&c_a;&c_v;&c_y;</trd>,
        <trd>&c_g;&c_l;&c_ja;&c_n;&c_c;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
      </trdgrp>
  <trdgrp lng="de">
        <trd>Glace-</trd>,
        <trd>Glanz-</trd>
      </trdgrp>
  <trd lng="en">glossy</trd>
  <trd lng="fr">glac&eacute;<klr tip="ind"> (recouvert d'un enduit
          brillant)</klr></trd>
  <trdgrp lng="hu">
        <trd>f&eacute;nyezett</trd>,
        <trd>glasz&eacute;</trd>
      </trdgrp>
  <trdgrp lng="nl">
        <trd>glac&eacute;-</trd>,
        <trd>blinkend</trd>
      </trdgrp>
  <trdgrp lng="pt">
        <trd>glac&ecirc;</trd>, 
        <trd>lustroso</trd> 
      </trdgrp>
  <trdgrp lng="ru">
        <trd>&c_g;&c_l;&c_ja;&c_n;&c_c;&c_je;&c_v;&c_y;&c_j;</trd>,
        <trd>&c_l;&c_o;&c_shch;&c_jo;&c_n;&c_y;&c_j;</trd>
      </trdgrp>
  <trdgrp lng="sk">
    <trd>glacé</trd>,
    <trd>hladený</trd>,
    <trd>leštený</trd>
  </trdgrp>
</drv>

    <drv mrk="glace.0igi">
      <kap><tld/>igi</kap>
      <snc>
        <dif>
          Briligi per glatigo.
        </dif>
        <refgrp tip="vid">
          <ref cel="glazur.0i">glazuri</ref>.
        </refgrp>
      </snc>
  <trd lng="be">&c_g;&c_l;&c_ja;&c_n;&c_c;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
        <trd>glacieren</trd>,
        <trd>satinieren</trd>,
        <trd>mit <ind>Hochglanz</ind> versehen</trd>,
        <trd>auf Hochglanz <ind>polieren</ind></trd>
      </trdgrp>
  <trd lng="fr">glacer<klr tip="ind"> (recouvrir d'un enduit
          brillant)</klr></trd>
  <trd lng="hu">f&eacute;nyez<klr tip="ind"> (pl. b&odblac;rt)</klr></trd>
  <trd lng="nl">glaceren</trd>
  <trdgrp lng="pt">
        <trd>dar <ind>brilho</ind></trd>, 
        <trd>tornar <ind>brilhante</ind></trd> 
      </trdgrp>
  <trdgrp lng="ru">
        <trd>&c_g;&c_l;&c_ja;&c_n;&c_c;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_n;&c_a;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol; &c_g;&c_l;&c_ja;&c_n;&c_je;&c_c;</trd>
      </trdgrp>
  <trd lng="sk">pokryť polevou <klr>(tortu)</klr></trd>
</drv>

    <drv mrk="glace.mal0a">
      <kap>mal<tld/>a</kap>
      <snc>
        <dif>
          Senbrila (pri surfaco), <ref cel="difuz.0i">difuzanta</ref>
          la reflektatan lumon:
          <ekz>
            la mal<tld/>aj ekranoj ne havas rebrilojn kaj malpli
            lacigas la okulojn.
          </ekz>
        </dif>
      </snc>
      <trd lng="en">matt(e)</trd>
      <trd lng="ru">&c_m;&c_a;&c_t;&c_o;&c_v;&c_y;&c_j;</trd>
    </drv>


  </art>
