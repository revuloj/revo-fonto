<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>glace</rad>/a
<fnt>Z</fnt></kap>
<drv mrk="glace.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Briligita per glatigo (p.p. papero, ledo ktp):
      <ekz>
        <tld/>aj gantoj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Glace-</trd>,
    <trd>Glanz-</trd>
  </trdgrp>
  <trd lng="fr">glac&eacute;<klr tip="ind"> (recouvert d'un enduit
  brillant)</klr></trd>
  <trdgrp lng="nl">
    <trd>glac&eacute;-</trd>,
    <trd>blinkend</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_l;&c_ja;&c_n;&c_c;&c_je;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_l;&c_o;&c_shch;&c_jo;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="glace.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Briligi per glatigo.
    </dif>
    <refgrp tip="vid">
      <ref cel="glazur.0i">glazuri</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>glacieren</trd>,
    <trd>satinieren</trd>,
    <trd>mit <ind>Hochglanz</ind> versehen</trd>,
    <trd>auf Hochglanz <ind>polieren</ind></trd>
  </trdgrp>
  <trd lng="fr">glacer<klr tip="ind"> (recouvrir d'un enduit
  brillant)</klr></trd>
  <trd lng="nl">glaceren</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_l;&c_ja;&c_n;&c_c;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol; &c_g;&c_l;&c_ja;&c_n;&c_je;&c_c;</trd>
  </trdgrp>
</drv>
</art>
