<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>transept</rad>/o
</kap>
<drv mrk="transe.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ARKI</uzo>
  <snc>
    <dif>
      Parto de la krucformaj pre&gcirc;ejoj, kiu rektangule kruci&gcirc;as kun la navo
      kaj formas la brakojn de la kruco.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_a;&c_n;&c_s;&c_e;&c_p;&c_t;</trd>
  <trdgrp lng="de">
    <trd>Transept</trd>,
    <trd>Querschiff</trd>
  </trdgrp>
  <trd lng="en">transept</trd>
  <trd lng="hu">kereszthaj&oacute;</trd>
  <trd lng="nl">transept</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_p;&c_je;&c_r;&c_je;&c_ch;&c_n;&c_y;&c_j; &c_n;&c_je;&c_f;</trd>,
    <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_je;&c_p;&c_t;</trd>
  </trdgrp>
</drv>
</art>
