<kap>
  <ofc>2</ofc>
  <rad>transept</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="transe.0o">
  <kap><tld/>o</kap>
  <snc mrk="transe.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Parto de krucforma pre&gcirc;ejo, kiu orte kruci&gcirc;as kun la
      navo kaj formas la brakojn de la kruco:
      <ekz>
        en la norda parto de la <tld/>o, oni
        renkontas <klr>...</klr> la <ctl>Nigran
        Virgulinon</ctl>
        <fnt><aut>G. Waringhien</aut>, <vrk>Ni kaj &Gcirc;i</vrk>, 
        <lok>1972</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_a;&c_n;&c_s;&c_e;&c_p;&c_t;</trd>
  <trd lng="ca">transsepte</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;&iacute;&ccaron;n&aacute; lo&dcaron; kostela</trd>,
    <trd>transept</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Transept</trd>,
    <trd>Querschiff</trd>
  </trdgrp>
  <trd lng="en">transept</trd>
  <trd lng="fr">transept</trd>
  <trd lng="hu">kereszthaj&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x30C8;&#x30E9;&#x30F3;&#x30BB;&#x30D7;&#x30C8;</trd>,
    <trd>&#x4EA4;&#x5DEE;&#x5ECA;  <pr>&#x3053;&#x3046;&#x3055;&#x5ECA;</pr></trd>
  </trdgrp>
  <trd lng="nl">transept</trd>
  <trdgrp lng="pl">
    <trd>transept</trd>,
    <trd>nawa poprzeczna</trd>,
    <trd>nawa krzy&zdot;owa</trd>
  </trdgrp>
  <trd lng="pt">transcepto</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_p;&c_je;&c_r;&c_je;&c_ch;&c_n;&c_y;&c_j; &c_n;&c_je;&c_f;</trd>,
    <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_je;&c_p;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">kr&iacute;&zcaron;ov&aacute; lo&dcaron; chr&aacute;mov&aacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43F;&#x435;&#x440;&#x435;&#x447;&#x43D;&#x438;&#x439; &#x43D;&#x435;&#x444;</trd>,
    <trd>&#x442;&#x440;&#x430;&#x43D;&#x441;&#x435;&#x43F;&#x442;</trd>
  </trdgrp>
</drv>
</art>
