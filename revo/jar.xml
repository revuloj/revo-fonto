<drv mrk="jar.lum0o">
  <kap>lum<tld/>o</kap>
  <snc mrk="jar.lum0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Praktika <ref tip="lst" cel="unu.mezur0o" lst="voko:longoj">mezurunuo</ref> de
      <ref cel="long.0o.FIZ">longo</ref>, uzata por mezuri grandegajn
      interastrajn distancojn, egala al 9,4605278.10<sup>15</sup>
      <ref tip="prt" cel="metr.0o.unuo">metroj</ref>; simbolo l.j.:
      <ekz>
        unu lum<tld/>o estas la distanco, kiun trairas lumo en vakuo dum
        unu <tld/>o
        <fnt>
          <bib>Viki</bib>
          <vrk><url>https://eo.wikipedia.org/?curid=17661</url></vrk>
          <lok><url ref="https://eo.wikipedia.org/?curid=17661">Lumjaro</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu objekto orbitas junan stelon en la konstelacio Lupo, je 400
        &gcirc;is 500 lum<tld/>oj for
        <fnt>
          <bib>Monato</bib>
          <aut>Oliver Kim</aut>
          <vrk><url ref="&Monato;/2006/008796.php">Unua
          fotografa&jcirc;o de ekstersunsistema planedo</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_v;&c_je;&c_t;&c_l;&c_a;&c_v;&c_y; 
<ind>&c_g;&c_o;&c_d;</ind></trd>
  <trd lng="ca">any <ind>llum</ind></trd>
  <trd lng="cs">sv&ecaron;teln&yacute; rok</trd>
  <trd lng="de">Lichtjahr</trd>
  <trd lng="en">light-year</trd>
  <trd lng="es">a&ntilde;o luz</trd>
  <trd lng="fr">ann&eacute;e-lumi&egrave;re</trd>
  <trd lng="hu">f&eacute;ny&eacute;v</trd>
  <trd lng="id"><ind>tahun</ind> cahaya</trd>
  <trd lng="it">anno luce</trd>
  <trd lng="nl">lichtjaar</trd>
  <trd lng="no">lys&aring;r</trd>
  <trd lng="ru">&c_s;&c_v;&c_je;&c_t;&c_o;&c_v;&c_o;&c_j; &c_g;&c_o;&c_d;
</trd>
  <trd lng="sk">sveteln&yacute; rok</trd>
  <trd lng="sv">ljus&aring;r</trd>
</drv>

