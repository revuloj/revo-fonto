<drv mrk="jar.cent0o">
  <kap>cent<tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>
        <fnt>
          <bib>EE</bib>
        </fnt>
  </kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="jar.cent0o.KAL">
    <dif>
      <ref tip="dif" cel="cent.jar0o"><tld lit="J"/>cento</ref>:
      <ekz>
        tutan homan vivon, e&ccirc; cent<tld/>ojn da&ubreve;ras la
        fruktoj de laborado de fruktoplantisto
