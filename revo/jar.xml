  <snc mrk="jar.0o.AST">
    <tezrad fak="AST"/>
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="lst" cel="dauxr.0o" lst="voko:da&ubreve;roj">Da&ubreve;ro</ref>
      dum kiu la tero faras kompletan rondiron
      &ccirc;irka&ubreve; la suno, proksimume 365 tagoj kaj 1
      kvarono:
      <ekz>
        en la da&ubreve;ro de du <tld/>oj
        <fnt><bib>F</bib>, <lok>&FE; 25</lok></fnt>;
      </ekz>
      <ekz>
        per la sa&gcirc;o aldoni&gcirc;os al vi <tld/>oj da
        vivo
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 9:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiom da <tld/>oj vi havas
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>?
      </ekz>
      <ekz>
        kiom<tld/>a estas la virino, eble tridek
        <fnt>
          <bib>Monato</bib>
          <aut>Julian Modest</aut>
          <vrk><url ref="&Monato;/2005/008708.php"
              >Patrino</url></vrk>
          <lok>2005</lok>
        </fnt>?
      </ekz>
      <ekz>
        konservi dek<tld/>an pacon
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        griza pro mult<tld/>eco;
      </ekz>
      <ekz>
        cent<tld/>a jubileo
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470;3</lok>
        </fnt>;
      </ekz>
      <ekz>
        cent<tld/>ulo
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>rok<klr> s&lstroke;oneczny</klr></trd>,
      <trd>rok<klr> astronomiczny</klr></trd>
