<drv mrk="jar.dekkelk0ulo">
  <kap>dekkelk<tld/>ulo</kap>
  <snc mrk="jar.dekkelk0ulo.KOMUNE">
    <dif>
      <ref tip="vid" cel="jun.0ulo">Junulo</ref> iom pli ol dekjara&gcirc;a, 
      proksimume 13 &gcirc;is 19-jara, 
<ref tip="vid" cel="adolesk.0ulo.KOMUNE">adoleskulo</ref>:
      <ekz>
        kiam li estis dekkelk<tld/>ulo, li konstruis funkciantan 
        telefonan sistemon
        <fnt>
          <aut>J. Casey</aut>,
          <vrk>
<url ref="http://www.skotlando.org/En%20Esperanto/Skota%20Kulturo/John%20Logie %20Baird.htm">
          John Logie Baird</url></vrk>,
          <lok>[vidita en 2008]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">adolescent</trd>
  <trd lng="en">teenager</trd>
  <trd lng="es">adolescente</trd>
  <trdgrp lng="id">
