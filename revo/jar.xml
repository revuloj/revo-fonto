<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>jar</rad>/o
</kap>
<drv mrk="jar.0o">
<kap><ofc>*</ofc><tld/>o</kap>
 <snc>
  <dif>
   Divido de la da&ubreve;ro: tempo dum kiu la tero faras kompletan rondiron
   &ccirc;irka&ubreve; la suno (proksimume 365 tagoj k 1 kvarono): <ekz>per la
   sa&gcirc;o aldoni&gcirc;os al vi <tld/>oj da vivo<fnt>Z</fnt>;</ekz>
   <ekz>kiom da <tld/>oj vi havas?</ekz>
   <ekz>kiom<tld/>a vi estas?</ekz>
   <ekz>mi estas dek<tld/>a;</ekz>
   <ekz>griza pro mult<tld/>eco;</ekz>
   <ekz>cent<tld/>a jubileo;</ekz>
   <ekz>cent<tld/>ulo.</ekz>
  </dif>
  <trd lng="de">Jahr</trd>
 </snc>
 <trd lng="be">&c_g;&c_o;&c_d;</trd>
 <trd lng="cs">rok</trd>
 <trd lng="en">year</trd>
 <trd lng="es">a&ntilde;o</trd>
 <trd lng="it">anno</trd>
 <trd lng="ru">&c_g;&c_o;&c_d;</trd>
</drv>
<drv mrk="jar.0cento">
<kap><tld/>cento</kap>
 <snc>
  <dif>
   Da&ubreve;ro de 100 <tld/>oj: <ekz>la scienco multe progresis dum la XIX
   <tld/>cento.</ekz>
  </dif>
  <trd lng="de">Jahrhundert</trd>
 </snc>
 <trd lng="be">&c_s;&c_t;&c_a;&c_g;&c_o;&c_d;&c_z;&c_mol;&c_d;&c_z;&c_je;</trd>
 <trd lng="en">century</trd>
 <trd lng="es">siglo</trd>
 <trd lng="it">secolo</trd>
 <trdgrp lng="ru">
   <trd>&c_s;&c_t;&c_o;&c_l;&c_je;&c_t;&c_i;&c_je;</trd>,
   <trd>&c_v;&c_je;&c_k;</trd>
 </trdgrp>
</drv>
<drv mrk="jar.0libro">
<kap><tld/>libro</kap>
 <snc>
  <dif>
   Libro enhavanta &ccirc;iujn interesajn sciigojn pri la &jcirc;us pasinta <tld/>o. 
 </dif>
  <trd lng="de">Jahrbuch</trd>
 </snc>
 <trd lng="ru">&c_je;&c_zh;&c_je;&c_g;&c_o;&c_d;&c_n;&c_i;&c_k;</trd>
</drv>
<drv mrk="jar.cxiu0a">
<kap>&ccirc;iu<tld/>a</kap>
 <snc>
  <dif>
   Revenanta a&ubreve; rekomenci&gcirc;anta &ccirc;iun <tld/>on:
   <ekz>&ccirc;iu<tld/>a vegeta&jcirc;o, ceremonio.</ekz>
  </dif>
 </snc>
 <trd lng="be">&c_sh;&c_t;&c_o;&c_g;&c_a;&c_d;&c_o;&c_v;&c_y;</trd>
 <trd lng="de">j&auml;hrlich</trd>
 <trd lng="en">annual</trd>
 <trd lng="es">anual</trd>
 <trd lng="it">annuale</trd>
 <trd lng="ru">&c_je;&c_zh;&c_je;&c_g;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>
</drv>
<drv mrk="jar.duon0o0duono">
<kap>duon<tld/>o, <tld/>duono</kap>
 <snc>
  <dif>
   Sesmonata duono da <tld/>o. 
  </dif>
 </snc>
 <trd lng="be">&c_p;&c_a;&c_w;&c_g;&c_o;&c_d;&c_z;&c_mol;&c_d;&c_z;&c_je;</trd>
  <trd lng="de">Halbjahr</trd>
 <trd lng="es">semestre</trd>
 <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_g;&c_o;&c_d;&c_i;&c_je;</trd>
</drv>
<drv mrk="jar.super0o">
<kap>super<tld/>o</kap>
 <snc>
  <dif>
   <tld/>o enhavanta 366 tagojn.  
 </dif>
 </snc>
 <trd lng="be">&c_v;&c_ib;&c_s;&c_a;&c_k;&c_o;&c_s;&c_n;&c_y; &c_g;&c_o;&c_d;</trd>
 <trd lng="de">Schaltjahr</trd>
 <trd lng="en">leap year</trd>
 <trd lng="es">a&ntilde;o bisiesto</trd>
 <trd lng="it">anno bisestile</trd>
 <trd lng="ru">&c_v;&c_i;&c_s;&c_o;&c_k;&c_o;&c_s;&c_n;&c_y;&c_j; &c_g;&c_o;&c_d;</trd>
</drv>
</art>
