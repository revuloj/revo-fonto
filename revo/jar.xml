<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>jar</rad>/o
</kap>
<drv mrk="jar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">KAL</uzo>
    <dif>
      Tempo dum kiu la tero faras kompletan rondiron
      &ccirc;irka&ubreve; la suno (proksimume 365 tagoj kaj 1 kvarono):
      <ekz>
        en la da&ubreve;ro de du <tld/>oj
        <fnt><bib>F</bib>, <lok>&FE; 25</lok></fnt>;
      </ekz>
      <ekz>
        per la sa&gcirc;o aldoni&gcirc;os al vi <tld/>oj da vivo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kiom da <tld/>oj vi havas?
      </ekz>
      <ekz>
        kiom<tld/>a vi estas?
      </ekz>
      <ekz>
        mi estas dek<tld/>a;
      </ekz>
      <ekz>
        griza pro mult<tld/>eco;
      </ekz>
      <ekz>
        cent<tld/>a jubileo;
      </ekz>
      <ekz>
        cent<tld/>ulo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_o;&c_d;</trd>
  <trd lng="cs">rok</trd>
  <trd lng="de">Jahr</trd>
  <trdgrp lng="el">
    <trd>&epsilon_ton;&tau;&omikron;&sigma_fina;</trd>,
    <trd>&chi;&rho;&omikron_ton;&ny;&omikron;&sigma_fina;</trd>
  </trdgrp>
  <trd lng="en">year</trd>
  <trd lng="es">a&ntilde;o</trd>
  <trd lng="fr">ann&eacute;e</trd>
  <trd lng="hu">&eacute;v</trd>
  <trd lng="it">anno</trd>
  <trd lng="nl">jaar</trd>
  <trd lng="pt">ano</trd>
  <trd lng="ru">&c_g;&c_o;&c_d;</trd>
  <trd lng="sk">rok</trd>
  <trd lng="sv">&aring;r</trd>
  <trd lng="vo">yel</trd>
</drv>

<drv mrk="jar.0libro">
  <kap><tld/>libro</kap>
  <snc>
    <dif>
      Libro enhavanta &ccirc;iujn interesajn sciigojn pri la &jcirc;us pasinta <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Jahrbuch</trd>
  <trd lng="es">anuario</trd>
  <trd lng="fr">annuaire<klr> (livre de l'ann&eacute;e)</klr></trd>
  <trd lng="hu">&eacute;vk&ouml;nyv</trd>
  <trd lng="nl">jaarboek</trd>
  <trd lng="ru">&c_je;&c_zh;&c_je;&c_g;&c_o;&c_d;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">ro&ccaron;enka</trd>
  <trd lng="sv">&aring;rsbok</trd>
</drv>

<drv mrk="jar.cent0o">
  <kap>cent<tld/>o<fnt>Z</fnt></kap>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <ref tip="dif" cel="cent.jar0o"><tld/>cento</ref>
  </snc>
  <trd lng="hu">&eacute;vsz&aacute;zad</trd>
</drv>

<drv mrk="jar.cxiu0a">
  <kap>&ccirc;iu<tld/>a</kap>
  <snc>
    <dif>
      Revenanta a&ubreve; rekomenci&gcirc;anta &ccirc;iun <tld/>on:
      <ekz>
        &ccirc;iu<tld/>a vegeta&jcirc;o, ceremonio.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_t;&c_o;&c_g;&c_a;&c_d;&c_o;&c_v;&c_y;</trd>
  <trd lng="de">j&auml;hrlich</trd>
  <trdgrp lng="el">
    <trd>&epsilon;&tau;&eta_ton;&sigma;&jota;&omikron;&sigma_fina;</trd>,
    <trd>&chi;&rho;&omikron;&ny;&jota;&alfa_ton;&tau;&jota;&kappa;&omikron;&sigma_fina;</trd>
  </trdgrp>
  <trd lng="en">annual</trd>
  <trd lng="es">anual</trd>
  <trd lng="fr">annuel</trd>
  <trdgrp lng="hu">
    <trd>&eacute;venk&eacute;nti</trd>,
    <trd>&eacute;vel&odblac;</trd>
  </trdgrp>
  <trd lng="it">annuale</trd>
  <trd lng="nl">jaarlijks</trd>
  <trd lng="pt">anual</trd>
  <trd lng="ru">&c_je;&c_zh;&c_je;&c_g;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">ka&zcaron;doro&ccaron;n&yacute;</trd>
  <trd lng="sv">&aring;rlig</trd>
</drv>

<drv mrk="jar.duon0o0duono">
  <kap>duon<tld/>o, <tld/>duono</kap>
  <snc>
    <uzo tip="fak">KAL</uzo>
    <dif>
      Sesmonata duono da <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_w;&c_g;&c_o;&c_d;&c_z;&c_mol;&c_d;&c_z;&c_je;</trd>
  <trd lng="de">Halbjahr</trd>
  <trd lng="es">semestre</trd>
  <trd lng="fr">semestre</trd>
  <trd lng="hu">f&eacute;l&eacute;v</trd>
  <trd lng="nl">semester</trd>
  <trd lng="pt">semestre</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_g;&c_o;&c_d;&c_i;&c_je;</trd>
  <trd lng="sk">polrok</trd>
  <trd lng="sv">halv&aring;r</trd>
</drv>

<drv mrk="jar.mil0o">
  <kap><ofc>*</ofc>mil<tld/>o</kap>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <ref tip="dif" cel="mil.jar0o"><tld/>milo</ref>
    <ekz>
      mil <tld/>oj (a&ubreve; milo da <tld/>oj) faras mil<tld/>on
      <fnt><bib>F</bib>, <lok>&FE; 14</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="hu">&eacute;vezred</trd>
</drv>

<drv mrk="jar.prov0o">
  <kap>prov<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="J"/>o, dum kiu oni ion eksperimentis;
      <klr>(plurale, p.p. profesiulo)</klr> la <tld/>oj, dum kiuj
      li lernis kaj akiris la profesiajn kapablojn:
      <ekz>
        &ccirc;i-somere &leftquot;Kvinpetalo&rightquot; i&gcirc;os dudekjara
        kaj fakte estos funkcianta de dek tri jaroj, enkalkulante
        la prov<tld/>on 1984
        <fnt><vrk>Eventoj</vrk><lok><url
        ref="http://www.eventoj.hu/arkivo/eve-128.htm">1997-6(128)</url></lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>p&eacute;riode d'apprentissage</trd>,
    <trd>p&eacute;riode de formation</trd>
  </trdgrp>
  <trd lng="hu">pr&oacute;ba&eacute;v</trd>
</drv>

<drv mrk="jar.super0o">
  <kap>super<tld/>o</kap>
  <snc>
    <uzo tip="fak">KAL</uzo>
    <dif>
      <tld lit="J"/>o enhavanta 366 tagojn.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_s;&c_a;&c_k;&c_o;&c_s;&c_n;&c_y; &c_g;&c_o;&c_d;</trd>
  <trd lng="de">Schaltjahr</trd>
  <trd lng="en">leap year</trd>
  <trd lng="es">a&ntilde;o bisiesto</trd>
  <trd lng="fr">ann&eacute;e bissextile</trd>
  <trd lng="it">anno bisestile</trd>
  <trd lng="hu">sz&ouml;k&odblac;&eacute;v</trd>
  <trd lng="nl">schrikkeljaar</trd>
  <trd lng="pt">ano bissexto</trd>
  <trd lng="ru">&c_v;&c_i;&c_s;&c_o;&c_k;&c_o;&c_s;&c_n;&c_y;&c_j; &c_g;&c_o;&c_d;</trd>
  <trd lng="sv">skott&aring;r</trd>
</drv>
</art>
