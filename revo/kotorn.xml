<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kotorn</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kotorn.0o">
  <kap><tld/>o</kap>
  <snc mrk="kotorn.0o.TEKS">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">TEKS</uzo>
    <dif>
      &Scirc;uo de antikvaj tragediaj aktoroj, kun tre alta plando:
      <ekz>
        ordonu al ili porti &ccirc;emizojn malanta&ubreve; la vestoj, kaj
<tld/>ojn
        en la piedoj
        <fnt>
        <aut>Herodoto, trad. S. Sarafian</aut>,
        <vrk>Historio</vrk>,
        <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_u;&c_r;&c_n;</trd>
  <trdgrp lng="de">
     <trd>Kothurn</trd>,
     <trd>hoher B&uuml;hnenschuh</trd>
  </trdgrp>
  <trd lng="fr">cothurne</trd>
  <trd lng="hu">koturnus</trd>
  <trdgrp lng="pt">
    <trd>coturno</trd>,
    <trd>chapim</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_t;&c_u;&c_r;&c_n;</trd>
</drv>
</art>

