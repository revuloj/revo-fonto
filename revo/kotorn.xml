    <trd>Kothurn</trd>,
    <trd>hoher B&uuml;hnenschuh</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cothurnus</trd>,
    <trd>buskin</trd>
  </trdgrp>
  <trd lng="fr">cothurne</trd>
  <trd lng="hu">koturnus</trd>
  <trd lng="ja">&#x539A;&#x5E95;&#x306E;&#x534A;&#x9577;&#x9774;  <pr>&#x3053;&#x3046;&#x305D;&#x3053;&#x306E;&#x306F;&#x3093;&#x306A;&#x304C;&#x3050;&#x3064;</pr></trd>
  <trd lng="pl">koturn</trd>
  <trdgrp lng="pt">
    <trd>coturno</trd>,
    <trd>chapim</trd>
  </trdgrp>
  <trd lng="ro">coturn</trd>
  <trd lng="ru">&c_k;&c_o;&c_t;&c_u;&c_r;&c_n;</trd>
  <trd lng="sk">koturn</trd>
  <trd lng="zh">&#x97A8; <pr>h&#xE9;</pr></trd>
</drv>
