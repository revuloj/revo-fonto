<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>gardeni</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="garden.0o">
  <kap><tld/>o</kap>
  <snc mrk="garden.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      &Ccirc;iamverda arbeto el malsekaj tropikaj regionoj kun belaj 
      dikpetalaj floroj kaj brilaj malhelverdaj folioj,
      genro el familio <ref tip="super" cel="rubi.0oj.BOT">rubiacoj</ref>
      (<trd lng="la">Gardenia</trd>):
      <ekz>
        la italo alvenis nokti&gcirc;e kun <tld/>o en la butontruo de la 
        jakfaldo
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>;
      </ekz>
      <ekz>
        <ctl>Du <tld/>oj</ctl>
        <klr>(novelo de M. Fern&aacute;ndez)</klr>
        <fnt>
          <aut>M. Fern&aacute;ndez</aut>,
          <vrk>Du gardenioj</vrk>,
          <lok><url ref="http://osiek.org/lagazeto/LG-66/index.html">
          La Gazeto, 1996-10, numero 66a</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_d;&c_e;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="de">Gardenie</trd>
  <trd lng="el">&gamma;&alfa;&rho;&delta;&epsilon_ton;&ny;&jota;&alfa;</trd>
  <trd lng="es">gardenia</trd>
  <trd lng="fr">gard&eacute;nia</trd>
  <trd lng="hu">gard&eacute;nia</trd>
  <trd lng="it">gardenia</trd>
  <trd lng="nl">gardeniastruik</trd>
  <trd lng="pt">gard&ecirc;nia</trd>
  <trd lng="ru">&c_g;&c_a;&c_r;&c_d;&c_je;&c_n;&c_i;&c_ja;</trd>
</drv>
</art>
