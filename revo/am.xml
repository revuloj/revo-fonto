<drv mrk="am.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="am.0i.iun">
    <dif>
      Havi inklinon por iu, &scirc;ati kare iun:
      <ekz>
        li <tld/>as tiun &ccirc;i knabinon pro &scirc;ia beleco kaj
        boneco<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ne bela estas <tld/>ata sed <tld/>ata estas
        bela<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        mia plej <tld/>ata infano
        <fnt><bib>Fab1</bib>, <lok>&Gcirc;ardeno de la paradizo</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="favor.0i">favori 2</ref>.
    </refgrp>
    <trd lng="bg">&c_o;&c_b;&c_i;&c_ch;&c_a;&c_m;</trd>
    <trdgrp lng="nl">
      <trd>houden van</trd>,
      <trd>liefhebben</trd>,
      <trd>beminnen</trd>
    </trdgrp>
  </snc>
  <snc mrk="am.0i.ion">
    <dif>
      &Scirc;ati, volonte uzi ion:
      <ekz>
        <tld/>i kafon, viandon;
      </ekz>
      <ekz>
        <tld/>i esti la&ubreve;data<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kiu <tld/>as honoron, <tld/>u laboron<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        mono <tld/>as kalkulon<fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="frand.0i">frandi</ref>.
    </refgrp>
    <trdgrp lng="nl">
      <trd>houden van</trd>,
      <trd>liefhebben</trd>,
      <trd>beminnen</trd>
    </trdgrp>
  </snc>
  <snc mrk="am.0i.agon">
    <dif>
      Volonte fari ion:
      <ekz>
        mi <tld/>as vivi alie ol &ccirc;iuj
        <fnt><bib>Fab2</bib>, <lok>La ombro</lok></fnt>;
      </ekz>
      <ekz>
        vid<tld/>a turismo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi <tld/>as promeni en arbaro;
      </ekz>
      <ekz>
        li <tld/>as danci, fanfaroni pri sia forto<fnt>Z</fnt>;
      </ekz>
      <ekz>
        vi <tld/>as preni, <tld/>u redoni<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="nl">
      <trd>liefhebben</trd>,
      <trd>beminnen</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_l;&c_ju;&c_b;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>milovat</trd>,
    <trd>m&iacute;t r&aacute;d</trd>
  </trdgrp>
  <trd lng="de">lieben</trd>
  <trd lng="en">love</trd>
  <trd lng="es">amar</trd>
  <trd lng="fr">aimer</trd>
  <trd lng="it">amare</trd>
  <trd lng="tr">sevmek</trd>
  <trd lng="ru">&c_l;&c_ju;&c_b;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>milova&tcaron;</trd>,
    <trd>ma&tcaron; r&aacute;d</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#29233;</trd>,
    <trd><klr>tr. </klr>&#24859;<klr> [&agrave;i]</klr></trd>
  </trdgrp>
</drv>

