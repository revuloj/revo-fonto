<drv mrk="am.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="am.0i.iun">
    <uzo tip="klr">(iun)</uzo>
    <dif>
      Havi koran (kaj eventuale anka&ubreve; seksan) inklinon por iu:
      <ekz>
        li <tld/>as tiun &ccirc;i knabinon pro &scirc;ia beleco kaj
        boneco<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind>ne bela estas <tld/>ata, sed <tld/>ata estas
        bela</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">akit szeret&uuml;nk, sz&eacute;pnek tal&aacute;ljuk</trd>
      </ekz>
      <ekz>
        mia plej <tld/>ata infano
        <fnt><bib>Fab1</bib>, <lok>&Gcirc;ardeno de la paradizo</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="favor.0i">favori 2</ref>.
    </refgrp>
    <trd lng="bg">&c_o;&c_b;&c_i;&c_ch;&c_a;&c_m;</trd>
    <trdgrp lng="nl">
      <trd>houden van</trd>,
      <trd>liefhebben</trd>,
      <trd>beminnen</trd>
    </trdgrp>
  </snc>
  <snc mrk="am.0i.sxati">
    <subsnc mrk="am.0i.ion">
      <uzo tip="klr">(ion)</uzo>
      <dif>
        Volonte uzi tion:
        <ekz>
          <tld/>i kafon, viandon;
        </ekz>
        <ekz>
          <ind>kiu <tld/>as honoron, <tld/>u laboron</ind>
          <fnt><bib>PrV</bib></fnt>;
          <trd lng="hu">munka n&eacute;lk&uuml;l nincs tisztess&eacute;g</trd>
        </ekz>
        <ekz>
          <ind>mono <tld/>as kalkulon</ind>
          <fnt><bib>PrV</bib></fnt>.
          <trd lng="hu">a p&eacute;nz sz&aacute;molva j&oacute;</trd>
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="frand.0i">frandi</ref>.
      </refgrp>
      <trdgrp lng="nl">
        <trd>houden van</trd>,
        <trd>liefhebben</trd>,
        <trd>beminnen</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="am.0i.agon">
      <uzo tip="klr">(fari ion)</uzo>
      <dif>
        Volonte tion fari:
        <ekz>
          mi <tld/>as vivi alie ol &ccirc;iuj
          <fnt><bib>Fab2</bib>, <lok>La ombro</lok></fnt>;
        </ekz>
        <ekz>
          <tld/>i esti la&ubreve;data<fnt>Z</fnt>;
        </ekz>
        <ekz>
          mi <tld/>as promeni en arbaro;
        </ekz>
        <ekz>
          li <tld/>as danci, fanfaroni pri sia forto<fnt>Z</fnt>;
        </ekz>
        <ekz>
          vi <tld/>as preni, <tld/>u redoni
          <fnt><bib>PrV</bib></fnt>;
        </ekz>
        <ekz>
          vid<tld/>a turismo<fnt>Z</fnt>.
        </ekz>
      </dif>
      <trdgrp lng="nl">
        <trd>liefhebben</trd>,
        <trd>beminnen</trd>
      </trdgrp>
    </subsnc>
    <ref tip="sin" cel="sxat.0i.ami">&scirc;ati <sncref/></ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_l;&c_ju;&c_b;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>milovat</trd>,
    <trd>m&iacute;t r&aacute;d</trd>
  </trdgrp>
  <trd lng="de">lieben</trd>
  <trd lng="en">love</trd>
  <trd lng="es">amar</trd>
  <trd lng="fr">aimer</trd>
  <trdgrp lng="hu">
    <trd>szeret</trd>,
    <trd>kedvel</trd>
  </trdgrp>
  <trd lng="it">amare</trd>
  <trd lng="tr">sevmek</trd>
  <trd lng="ru">&c_l;&c_ju;&c_b;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>milova&tcaron;</trd>,
