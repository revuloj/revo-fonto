<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>am</rad>/i
</kap>
<drv mrk="am.0i">
   <kap><ofc>*</ofc><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc num="1">
     <dif>
       Havi inklinon por iu, &scirc;ati kare iun:
       <ekz>
         li <tld/>as tiun &ccirc;i knabinon pro &scirc;ia beleco kaj
         boneco<fnt>Z</fnt>;
       </ekz>
       <ekz>
         ne bela estas <tld/>ata sed <tld/>ata estas
         bela
         <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
       </ekz>
       <ekz>
         mia plej <tld/>ata infano
         <fnt>(<aut>Zamenhof</aut>: <vrk>Fabeloj de Andersen,
         &Gcirc;ardeno de la paradizo</vrk>)</fnt>.
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="favor.0i">favori 2</ref>.
     </refgrp>
   <trd lng="bg">&c_o;&c_b;&c_i;&c_ch;&c_a;&c_m;</trd>
   <trdgrp lng="nl">
     <trd>houden van</trd>,
     <trd>liefhebben</trd>,
     <trd>beminnen</trd>
   </trdgrp>
   </snc>
   <snc num="2">
     <dif>
       &Scirc;ati, volonte uzi ion:
       <ekz>
         <tld/>i kafon, viandon;
       </ekz>
       <ekz>
         <tld/>i esti la&ubreve;data<fnt>Z</fnt>;
       </ekz>
       <ekz>
         kiu <tld/>as honoron, <tld/>u laboron
         <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
       </ekz>
       <ekz>
         mono <tld/>as kalkulon
         <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="frand.0i">frandi</ref>.
     </refgrp>
   <trdgrp lng="nl">
     <trd>houden van</trd>,
     <trd>liefhebben</trd>,
     <trd>beminnen</trd>
   </trdgrp>
   </snc>
   <snc num="3">
     <dif>
       Volonte fari ion:
       <ekz>
         mi <tld/>as vivi alie ol &ccirc;iuj
         <fnt>(<aut>Zamenhof</aut>: <vrk>Fabeloj de Andersen,
         La ombro</vrk>)</fnt>;
       </ekz>
       <ekz>
         vid<tld/>a turismo<fnt>Z</fnt>;
       </ekz>
       <ekz>
         mi <tld/>as promeni en arbaro;
       </ekz>
       <ekz>
         li <tld/>as danci, fanfaroni pri sia forto<fnt>Z</fnt>;
       </ekz>
       <ekz>
         vi <tld/>as preni, <tld/>u redoni
         <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>.
       </ekz>
     </dif>
   <trdgrp lng="nl">
     <trd>liefhebben</trd>,
     <trd>beminnen</trd>
   </trdgrp>
   </snc>
   <trdgrp lng="cs">
     <trd>milovat</trd>,
     <trd>m&iacute;t r&aacute;d</trd>
   </trdgrp>
   <trd lng="de">lieben</trd>
   <trd lng="en">love</trd>
   <trd lng="es">amar</trd>
   <trd lng="fr">aimer</trd>
   <trd lng="it">amare</trd>
   <trd lng="tr">sevmek</trd>
   <trd lng="ru">&#1083;&#1102;&#1073;&#1080;&#1090;&#1100;</trd>
</drv>
<drv mrk="am.0o">
   <kap><tld/>o</kap>
   <snc>
     <dif>
       Inklino al persono a&ubreve; ideo:
       <ekz>
         havi, senti, porti<fnt>Z</fnt> <tld/>on al;
       </ekz>
       <ekz>
         oni ne forgesas facile sian unuan <tld/>on<fnt>Z</fnt>.
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="inklin.0o">korinklino</ref>,
       <ref cel="lig">korligi&gcirc;o</ref>,
       <ref cel="simpat.0o">simpatio</ref>.
     </refgrp>
   </snc>
   <trd lng="es">amor</trd>
   <trd lng="de">Liebe</trd>
   <trd lng="fr">amour</trd>
   <trd lng="it">amore</trd>
   <trd lng="nl">liefde</trd>
   <trd lng="ru">&#1083;&#1102;&#1073;&#1086;&#1074;&#1100;</trd>
   <trdgrp lng="tr">
     <trd>sevgi</trd>,
     <trd>a&scedil;k</trd>,
     <trd>sevda</trd>
   </trdgrp>
</drv>
<drv mrk="am.0acxi">
   <kap><tld/>a&ccirc;i, <tld/>a&jcirc;i<fnt>Z</fnt></kap>
   <snc>
     <uzo tip="klr">(Ev)</uzo>
     <dif>
       Sekse <tld/>i, amori.
     </dif>
   </snc>
   <trd lng="it">fare l'amore</trd>
   <trd lng="nl">neuken</trd>
</drv>
<drv mrk="am.0ajxo">
   <kap><tld/>a&jcirc;o</kap>
   <snc>
     <dif>
       <tld/>afero, <tld/>intrigo.
     </dif>
   </snc>
   <trdgrp lng="fr">
     <trd>liaison</trd>,
     <trd>aventure</trd></trdgrp>
   <trd lng="it">amore</trd>
   <trd lng="nl">liefdesaffaire</trd>
</drv>
<drv mrk="am.0ajxema">
   <kap><tld/>a&jcirc;ema</kap>
   <snc>
     <dif>
       <tld/>intrigema, flirtema, <tld/>induma.
     </dif>
   </snc>
   <trd lng="fr">dragueur</trd>
   <trd lng="nl">flirtziek</trd>
</drv>
<drv mrk="am.0ajxistino">
   <kap><tld/>a&jcirc;ist(in)o</kap>
   <snc>
     <dif>
       <tld/>donant(in)o, amorant(in)o, amorat(in)o.
     </dif>
   </snc>
   <trdgrp lng="fr">
     <trd>courtisane</trd>,
     <trd>gigolo</trd></trdgrp>
   <trd lng="nl">lief</trd>
</drv>
<drv mrk="am.0anto">
   <kap><tld/>ant(in)o</kap>
   <snc num="1">
     <dif>
       Tiu, kiu amike <tld/>as.
     </dif>
     <trd lng="nl">geliefde</trd>
   </snc>
   <snc num="2">
     <dif>
       Tiu, kiu sekse <tld/>as.
     </dif>
   <trdgrp lng="fr">
     <trd>amant</trd>,
     <trd>ma&icirc;tresse</trd>,
     <trd>petit(e) <ind>ami(e)</ind></trd>,
     <trd>amoureux</trd></trdgrp>
   <trd lng="nl">geliefde</trd>
   <trd lng="ru">&#1083;&#1102;&#1073;&#1086;&#1074;&#1085;&#1080;&#1082;</trd>
   </snc>
   <snc num="3">
     <dif>
       Tiu, kiu &scirc;atas, amatoro:
       <ekz>
         e&ccirc; por pomo putranta trovi&gcirc;as <tld/>anto
         <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>.
       </ekz>
     </dif>
   <trd lng="fr">amateur</trd>
   <trd lng="nl">liefhebber</trd>
   <trd lng="ru">&#1083;&#1102;&#1073;&#1080;&#1090;&#1077;&#1083;&#1100;</trd>
   </snc>
   <trdgrp lng="de"><trd>Liebender</trd>,
   <trd>Liebhaber</trd></trdgrp>
   <trd lng="es">amante</trd>
   </drv>
<drv mrk="am.0atajxo">
   <kap><tld/>ata&jcirc;o<fnt>Z</fnt></kap>
   <snc>
     <dif>
       Preferata okupi&gcirc;o
     </dif>
     <refgrp tip="sin">
       <ref cel="cxeval.0eto">&ccirc;evaleto</ref>.
     </refgrp>
   </snc>
   <trdgrp lng="nl"><trd>hobby</trd>,
   <trd>liefhebberij</trd></trdgrp>
   <trdgrp lng="fr">
     <trd>passe-temps</trd>,
     <trd>dada</trd>,
     <trd>marotte</trd>,
     <trd>hobby</trd></trdgrp>
   <trd lng="ru">&#1093;&#1086;&#1073;&#1073;&#1080;</trd>
</drv>
<drv mrk="am.0egi">
   <kap><tld/>egi</kap>
   <snc>
     <dif>
       Pasie <tld/>i.
     </dif>
     <refgrp tip="sin">
       <ref cel="ador.0i">adori 2</ref>.
     </refgrp>
   </snc>
   <trdgrp lng="fr">
     <trd>adorer</trd>,
     <trd>&ecirc;tre <ind>&eacute;pris</ind> (de)</trd>,
     <trd>raffoler (de)</trd></trdgrp>
   <trd lng="nl">aanbidden</trd>
   <trd lng="ru">&#1086;&#1073;&#1086;&#1078;&#1072;&#1090;&#1100;</trd>
</drv>
<drv mrk="am.0eti">
   <kap><tld/>eti</kap>
   <snc>
     <dif>
       Iom <tld/>i.
     </dif>
   </snc>
   <trdgrp lng="fr">
     <trd>s'<ind>int&eacute;resser</ind> (&agrave; quelqu'un)</trd>,
     <trd>avoir le <ind>b&eacute;guin</ind> (pour)</trd></trdgrp>
   <trd lng="nl">graag hebben</trd>
</drv>
<drv mrk="am.0inda">
   <kap><tld/>inda</kap>
   <snc>
     <dif>
       Meritanta <tld/>on:
       <ekz>
         por la ge<tld/>antoj &ccirc;io estas <tld/>inda
         en la <tld/>ato.
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="cxarm.0a">&ccirc;arma</ref>,
       <ref cel="placx.0a">pla&ccirc;a</ref>.
     </refgrp>
   </snc>
   <trd lng="de">liebensw&uuml;rdig</trd>
   <trd lng="es">amable</trd>
   <trdgrp lng="fr">
     <trd>aimable</trd>,
     <trd>charmant</trd></trdgrp>
   <trdgrp lng="it">
     <trd>amorevole</trd>,
     <trd>degno d'amore</trd>  </trdgrp>
   <trd lng="nl">beminnenswaardig</trd>
</drv>
<drv mrk="am.0indumi">
   <kap><tld/>indumi</kap>
   <snc>
     <gra><vspec>x</vspec></gra>
     <dif>
       Peni por pla&ccirc;i al vir(in)o:
       <ekz>
         <tld/>indumi iun, kun iu<fnt>Z</fnt>
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="flirt.0i">flirti</ref>,
       <ref cel="koket.0i">koketi</ref>.
     </refgrp>
   </snc>
   <trd lng="es">cortejar</trd>
   <trdgrp lng="fr">
     <trd>faire la <ind>cour</ind></trd>,
     <trd>flirter</trd>,
     <trd>draguer</trd></trdgrp>
   <trdgrp lng="it">
     <trd>corteggiare</trd>,
     <trd>fare la corte</trd></trdgrp>
   <trd lng="nl">flirten</trd>
   <trdgrp lng="ru">
     <trd>&#1092;&#1083;&#1080;&#1088;&#1090;&#1086;&#1074;&#1072;&#1090;&#1100;</trd>,
     <trd>&#1091;&#1093;&#1072;&#1078;&#1080;&#1074;&#1072;&#1090;&#1100;</trd>
   </trdgrp>
</drv>
<drv mrk="am.en0igxi">
   <kap>en<tld/>i&gcirc;i</kap>
   <snc>
     <dif>
       Eksenti fervoran <tld/>on por iu:
       <ekz>
         en<tld/>i&gcirc;i al<fnt>Z</fnt>, por iu<fnt>Z</fnt>,
         en iun<fnt>Z</fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="de">verlieben</trd>
   <trd lng="es">enamorarse</trd>
   <trd lng="fr">tomber <ind>amoureux</ind></trd>
   <trd lng="it">innamorarsi</trd>
   <trd lng="nl">verliefd worden</trd>
   <trd 
lng="ru">&#1074;&#1083;&#1102;&#1073;&#1083;&#1103;&#1090;&#1100;&#1089;&#1103;</trd>
</drv>
<drv mrk="am.ge0antoj">
   <kap>ge<tld/>antoj</kap>
   <snc>
     <dif>
       Paro da inter<tld/>antoj.
     </dif>
   </snc>
   <trd lng="es">amantes</trd>
   <trd lng="fr">amants</trd>
   <trd lng="nl">verliefden</trd>
   <trd 
lng="ru">&#1074;&#1083;&#1102;&#1073;&#1083;&#1105;&#1085;&#1085;&#1099;&#1077;</trd>
</drv>
<drv mrk="am.mal0o">
   <kap>mal<tld/>o</kap>
   <snc num="1">
     <dif>
       Pasio, kiu inklinigas al malbondeziro a&ubreve; malbonago kontra&ubreve;
       iu.
     </dif>
   </snc>
   <snc num="2">
     <dif>
       Forta  malinklino kontra&ubreve; io
     </dif>
     <refgrp tip="sin">
       <ref cel="abomen">abomeno</ref>,
       <ref cel="antipa.0o">antipatio</ref>,
       <ref cel="nauxz.0o">na&ubreve;zo</ref>,
       <ref cel="favor">malfavoro</ref>.
     </refgrp>
   </snc>
   <trd lng="de">Hass</trd>
   <trd lng="es">odio</trd>
   <trd lng="fr">haine</trd>
   <trd lng="it">odio</trd>
   <trd lng="nl">haat</trd>
   <trd lng="tr">nefret</trd>
   <trd 
lng="ru">&#1085;&#1077;&#1085;&#1072;&#1074;&#1080;&#1089;&#1090;&#1100;</trd>
</drv>
<drv mrk="am.pli0i">
   <kap>pli<tld/>i</kap>
   <snc>
     <dif>
       Preferi.
     </dif>
   </snc>
   <trd lng="nl">liever hebben</trd>
</drv>
<drv mrk="am.0ovendistino">
   <kap><tld/>ovendistino</kap>
   <snc>
     <dif>
       Virino, kiu metie amoras, publikulino.
     </dif>
   </snc>
   <trd lng="de">Nutte</trd>
   <trd lng="es">prostituta</trd>
   <trd lng="fr">prostitu&eacute;e</trd>
   <trd lng="it">prostituta</trd>
   <trd lng="nl">prostituee</trd>
   <trd 
lng="ru">&#1087;&#1088;&#1086;&#1089;&#1090;&#1080;&#1090;&#1091;&#1090;&#1082;&#1072;</trd>
   <trd lng="tr">orospu</trd>
</drv>
<drv mrk="am.glor0o">
   <kap>glor<tld/>o</kap>
   <snc>
     <dif>
       <tld/>o al gloro.
     </dif>
   </snc>
   <trd lng="fr">ambition</trd>
   <trd lng="nl">eerzucht</trd>
   <trd 
lng="ru">&#1095;&#1077;&#1089;&#1090;&#1086;&#1083;&#1102;&#1073;&#1080;&#1077;</trd>
</drv>
<drv mrk="am.hom0o">
   <kap>hom<tld/>o</kap>
   <snc>
     <dif>
       <tld/>o al niaj kunhomoj, al la tuthomaro.
     </dif>
     <refgrp tip="sin">
       <ref cel="filant2.0o">filantropio</ref>.
     </refgrp>
   </snc>
   <trd lng="de">Menschenliebe</trd>
   <trd lng="fr">philanthropie</trd>
   <trd lng="it">filantropia</trd>
   <trd lng="nl">filantropie</trd>
   <trd 
lng="ru">&#1095;&#1077;&#1083;&#1086;&#1074;&#1077;&#1082;&#1086;&#1083;&#1102;&#1073;&#1080;&#1077;</trd>
</drv>
<drv mrk="am.sin0o">
   <kap>sin<tld/>o</kap>
   <snc>
     <dif>
       Egoismo.
     </dif>
   </snc>
   <trdgrp lng="de">
      <trd>Selbstliebe</trd>,
      <trd>Eigenliebe</trd></trdgrp>
   <trd lng="fr">&eacute;go&iuml;sme</trd>
   <trd 
lng="ru">&#1089;&#1072;&#1084;&#1086;&#1083;&#1102;&#1073;&#1080;&#1077;</trd>
   <trdgrp lng="nl">
     <trd>ego&iuml;sme</trd>,
     <trd>eigenliefde</trd></trdgrp>
</drv>
</art>
