<drv mrk="am.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="am.0i.iun">
    <uzo tip="klr">(iun)</uzo>
    <dif>
      Havi koran (kaj eventuale anka&ubreve; seksan) inklinon por iu:
      <ekz>
        li <tld/>as tiun &ccirc;i knabinon pro &scirc;ia beleco kaj
        boneco
        <fnt><bib>F</bib>, <lok>&FE; 35</lok></fnt>;
      </ekz><ekz>
        <ind>ne bela estas <tld/>ata, sed <tld/>ata estas
        bela</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">akit szeret&uuml;nk, sz&eacute;pnek 
        tal&aacute;ljuk</trd>
      </ekz>
      <ekz>
        mia plej <tld/>ata infano
        <fnt><bib>Fab1</bib>, <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="favor.0i">favori</ref>,
      <ref cel="prefer.0i">preferi</ref>
    </refgrp>
    <trd lng="bg">&c_o;&c_b;&c_i;&c_ch;&c_a;&c_m;</trd>
    <trd lng="es">amar</trd>
    <trdgrp lng="nl">
      <trd>houden van</trd>,
      <trd>liefhebben</trd>,
      <trd>beminnen</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>kocha&cacute;</trd>,
      <trd>mi&lstroke;owa&cacute;</trd>
    </trdgrp>
    <trd lng="pt">amar</trd>
  </snc>
  <snc mrk="am.0i.sxati">
    <dif>
      <ref tip="dif" cel="sxat.0i.ami">&Scirc;ati<sncref/></ref>:
    </dif>
    <subsnc mrk="am.0i.ion">
      <uzo tip="klr">(ion)</uzo>
      <dif>
        Volonte uzi:
        <ekz>
          <tld/>i kafon, viandon;
        </ekz><ekz>
          <ind>kiu <tld/>as honoron, <tld/>u laboron</ind>
          <fnt><bib>PrV</bib></fnt>;
          <trd lng="hu">munka n&eacute;lk&uuml;l nincs 
          tisztess&eacute;g</trd>
        </ekz><ekz>
          <ind>mono <tld/>as kalkulon</ind>
          <fnt><bib>PrV</bib></fnt>.
          <trd lng="hu">a p&eacute;nz sz&aacute;molva j&oacute;</trd>
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="frand.0i">frandi</ref>
      </refgrp>
      <trdgrp lng="nl">
        <trd>houden van</trd>,
        <trd>liefhebben</trd>,
        <trd>beminnen</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="am.0i.agon">
      <uzo tip="klr">(fari ion)</uzo>
      <dif>
        Volonte fari:
        <ekz>
          En varmega tago mi <tld/>as promeni en arbaro
          <fnt><bib>F</bib>, <lok>&FE;34.</lok></fnt>;
        </ekz><ekz>
          mi <tld/>as vivi alie ol &ccirc;iuj
          <fnt><bib>Fab2</bib>, <lok>La ombro</lok></fnt>;
        </ekz>
        <ekz>
          &ccirc;iu homo <klr>[...]</klr> <tld/>as esti la&ubreve;data
          <fnt>
            <bib>Marta</bib>
          </fnt>;
        </ekz>
        <ekz>
          li <tld/>as danci;
        </ekz>
        <ekz>
          li <tld/>is fanfaroni per sia forto
          <fnt>
            <bib>Marta</bib>
          </fnt>;
        </ekz>
        <ekz>
          vi <tld/>as preni, <tld/>u redoni
          <fnt><bib>PrV</bib></fnt>;
        </ekz><ekz>
          vid<tld/>a turismo
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista en
          Washington en la 15a de a&ubreve;gusto 1910</vrk>
        </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="nl">
        <trd>liefhebben</trd>,
        <trd>beminnen</trd>
      </trdgrp>
    </subsnc>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_l;&c_ju;&c_b;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_o;&c_b;&c_i;&c_ch;&c_a;&c_m;</trd>
  <trd lng="ca">estimar</trd>
  <trdgrp lng="cs">
    <trd>m&iacute;t r&aacute;d</trd>,
    <trd>milovat</trd>,
    <trd>m&iacute;t v oblib&ecaron;</trd>
  </trdgrp>
  <trd lng="de">lieben</trd>
  <trd lng="en">love</trd>
  <trd lng="es">amar</trd>
  <trd lng="fr">aimer</trd>
  <trdgrp lng="hu">
    <trd>szeret</trd>,
    <trd>kedvel</trd>
  </trdgrp>
  <trd lng="he">&lamed;&alef;&he;&vav;&bet;</trd>
  <trd lng="id">[<ind>cinta</ind>] mencintai</trd>
  <trd lng="it">amare</trd>
  <trd lng="ku">hez kirin</trd>
  <trd lng="no">elske</trd>
  <trd lng="oc">aimar</trd>
  <trdgrp lng="pl">
    <trd>kocha&cacute;</trd>,
    <trd>uwielbia&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>pl&abreve;cea  a-&tcedil;i</trd>,
    <trd>iubi</trd>
  </trdgrp>                                     
  <trd lng="ru">&c_l;&c_ju;&c_b;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>ma&tcaron; r&aacute;d</trd>,
    <trd>milova&tcaron;</trd>,
    <trd>&lcaron;&uacute;bi&tcaron;</trd>
  </trdgrp>
  <trd lng="th">&#3619;&#3633;&#3585;</trd>
  <trd lng="tr">sevmek</trd>
  <trdgrp lng="zh">
    <trd>&#29233;</trd>,
    <trd><klr>tr. </klr>&#24859;<klr> [&agrave;i]</klr></trd>
