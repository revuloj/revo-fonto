<art mrk="$Id: mikrorganism.xml,v 1.15 2024/01/26 17:40:06 revo Exp $">
<kap>
  <rad>mikroorganism</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="mikrorganism.0o">
  <kap><tld/>o</kap>
  <snc mrk="mikrorganism.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      &Ccirc;ia tre malgranda, okule ne videbla viva&jcirc;o: bakterio, viruso, gisto, iuj algoj...:
      <ekz>
        malkonvena temperaturo povas forte &gcirc;eni la vivkapablon de la
        <tld/>oj
        <fnt>
          <aut>Mosonyi J.</aut>,
          <vrk><url ref="http://www.freeweb.hu/eventoj/steb/ingxenierado/ceteraj/klima/f874-p58.htm">
          Teknika disvolvo kaj novaj produktoj...</url></vrk>,
          <lok>Fokuso, 1987:4, p. 58a</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni mortigas la <tld/>ojn lavante niajn manojn
        <fnt>
          <aut>L. Kordylewski</aut>,
          <vrk>Rajtigita mortigado</vrk>,
          <lok><url ref="http://www.esperanto-chicago.org/newsletters/03_07.pdf">
          Saluton!, 2007-03</url></lok>
        </fnt>.
      </ekz>
    </dif>
<ref tip="sin" cel="mikrob.0o">mikrobo</ref>
<ref tip="sub" cel="cxel.unu0ulo">unu&ccirc;elulo</ref>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_k;&c_r;&c_a;&c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_m;</trd>
  <trd lng="ca">microorganisme</trd>
  <trd lng="cs">mikroorganizmus</trd>
  <trd lng="de">Mikroorganismus</trd>
  <trd lng="fr">microorganisme</trd>
  <trd lng="id">mikroorganisme</trd>
  <trd lng="ja">&#x5FAE;&#x751F;&#x7269;  <pr>&#x3073;&#x305B;&#x3044;&#x3076;&#x3064;</pr></trd>
  <trd lng="pl">mikroorganizm</trd>
  <trd lng="sk">mikroorganizmus</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_ib;&c_k;&c_r;&c_o;&c_o;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_m;</trd>,
    <trd>&c_m;&c_ib;&c_k;&c_r;&c_o;&c_b;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5FAE;&#x751F;&#x7269; <pr>w&#x113;ish&#x113;ngw&#xF9;</pr></trd>,
    <trd>&#x5FAE;&#x5C0F;&#x52A8;&#x690D;&#x7269; <pr>w&#x113;ixi&#x1CE;od&#xF2;ngzh&#xED;w&#xF9;</pr></trd>,
    <trd>&#x5FAE;&#x5C0F;&#x52D5;&#x690D;&#x7269; <pr>w&#x113;ixi&#x1CE;od&#xF2;ngzh&#xED;w&#xF9;</pr></trd>,
    <trd>&#x5FAE;&#x751F;&#x7269; <pr>w&#xE9;ish&#x113;ngw&#xF9;</pr></trd>
  </trdgrp>
</drv>
</art>

