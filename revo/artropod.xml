<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: artropod.xml,v 1.9 2023/10/28 08:33:09 revo Exp $">
<kap>
  <rad>artropod</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="artropod.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="artropod.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="artik.0uloj.ZOO">Artikulo</ref>:
      <ekz>
        en Karbonio <klr>[&#x2026;]</klr> la &ccirc;efaj lo&gcirc;antoj de
        firma tero estis <tld/>oj
        <fnt>
         <aut>V. Proch&aacute;zka, trad. J. Kavka</aut>,
         <vrk><url ref="http://gchmin.ic.cz/isaeproviz/popular/zkamokoliprahy.html">
         Paleobiologio kaj fosilioj de la centra Bohemio</url></vrk>,
         <lok>[2010?]</lok>
        </fnt>;
      </ekz>
      <ekz>
        trovi ekzempleron de la <tld/>o
        <fnt>
          <vrk><url ref="https://scivolemo.wordpress.com/2017/07/12/la-rego-de-la-maro/">La
          re&gcirc;o de la maro &#x2010; Scivolemo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o, kiu havas pli ol 6 piedojn, ne estas insekto
        <fnt>
          <vrk><url ref="http://www.literatura.bucek.name/brno/fischer_insektoj-en-niaj-hejmoj_140330.pdf">Insektoj
          en niaj hejmoj</url></vrk>
          <lok>literatura.bucek.name</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x7BC0;&#x80A2;&#x52D5;&#x7269; <pr>ji&#xE9;zh&#x12B;d&#xF2;ngw&#xF9;</pr></trd>,
    <trd>&#x8282;&#x80A2;&#x52A8;&#x7269; <pr>ji&#xE9;zh&#x12B;d&#xF2;ngw&#xF9;</pr></trd>,
    <trd>&#x7BC0;&#x652F;&#x52D5;&#x7269; <pr>ji&#xE9;zh&#x12B;d&#xF2;ngw&#xF9;</pr></trd>,
    <trd>&#x8282;&#x652F;&#x52A8;&#x7269; <pr>ji&#xE9;zh&#x12B;d&#xF2;ngw&#xF9;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_s;&c_u;&c_s;&c_t;&c_a;&c_v;&c_a;&c_n;&c_o;&c_g;&c_a;&c_je; <klr>(&c_n;&c_a;&c_z;.)</klr></trd>
  <trd lng="br">artropoded</trd>
  <trd lng="ca">artr&ograve;podes</trd>
  <trd lng="cs">&ccaron;lenovec</trd>
  <trdgrp lng="de">
    <trd>Gliederf&uuml;&szlig;er</trd>,
    <trd>Arthropode</trd>
  </trdgrp>
  <trd lng="en">arthropods</trd>
  <trd lng="es">artr&oacute;podos</trd>
  <trd lng="fr">arthropodes</trd>
  <trd lng="hu">&iacute;zeltl&aacute;b&uacute;ak</trd>
  <trd lng="it">artropodi</trd>
  <trd lng="ja">&#x7BC0;&#x8DB3;&#x52D5;&#x7269;  <pr>&#x305B;&#x3063;&#x305D;&#x304F;&#x3069;&#x3046;&#x3076;&#x3064;</pr></trd>
  <trd lng="pl">stawonogi</trd>
  <trd lng="pt">artr&oacute;pode</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_t;&c_r;&c_o;&c_p;&c_o;&c_d;&c_y;</trd>,
    <trd>&c_ch;&c_l;&c_je;&c_n;&c_i;&c_s;&c_t;&c_o;&c_n;&c_o;&c_g;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">&ccaron;lenovec</trd>
</drv>
</art>

