<art mrk="$Id: talk.xml,v 1.21 2024/05/10 08:31:28 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>talk</rad>/o
</kap>
<drv mrk="talk.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Blanka brila pulvoreca <ref tip="super" cel="minera.0o">mineralo</ref>, 
      <frm>Mg<sub>3</sub>[Si<sub>4</sub>O<sub>10</sub>(OH)<sub>2</sub>]</frm>, 
      grasa &ccirc;e palpado, uzata en medicino por pudroj ktp. 
      Steatito (sap&scirc;tono) konsistas precipe el <tld/>o:
      <ekz>
        la Ardezaj Alpoj havas stokejojn de fero- kaj kuproerco, magnezito, grafito kaj <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=568143">Orientaj Alpoj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        plej<klr>[ ]</klr>ofte la tajlorkreto estas el <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=531176">Tajlorkreto</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_l;&c_mol;&c_k;</trd>
  <trd lng="ca">talc</trd>
  <trdgrp lng="cs">
    <trd>klouzek</trd>,
    <trd>mastek</trd>,
    <trd>talek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Talk</trd>,
    <trd>Talkum</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>s&iacute;kpor</trd>,
    <trd>talkum</trd>
  </trdgrp>
  <trd lng="fr">talc</trd>
  <trdgrp lng="ja">
    <trd>&#x6ED1;&#x77F3;  <pr>&#x304B;&#x3063;&#x305B;&#x304D;</pr></trd>,
    <trd>&#x30BF;&#x30EB;&#x30AB;&#x30E0;&#x30D1;&#x30A6;&#x30C0;&#x30FC;</trd>
  </trdgrp>
  <trd lng="nl">talk</trd>
  <trd lng="pl">talk</trd>
  <trdgrp lng="pt">
   <trd>talco</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_l;&c_mol;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>mastenec</trd>,
    <trd>talek</trd>
  </trdgrp>
  <trd lng="sv">talk</trd>
  <trd lng="uk">&#x442;&#x430;&#x43B;&#x44C;&#x43A;</trd>
  <trdgrp lng="zh">
    <trd>&#x6ED1;&#x77F3; <pr>hu&#xE1;sh&#xED;</pr></trd>,
    <trd>&#x6ED1;&#x77F3;&#x7C89; <pr>hu&#xE1;sh&#xED;f&#x11B;n</pr></trd>
  </trdgrp>
</drv>
</art>
