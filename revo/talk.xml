<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>talk</rad>/o
</kap>
<drv mrk="talk.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Blanka brila pulvoreca mineralo, grasa &ccirc;e palpado, uzata en
      medicino por &scirc;mirado de la ha&ubreve;to.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_l;&c_mol;&c_k;</trd>
  <trd lng="de">Talk</trd>
  <trdgrp lng="hu">
    <trd>s&iacute;kpor</trd>,
    <trd>talkum</trd>
  </trdgrp>
  <trd lng="nl">talk</trd>
  <trd lng="pl">talk</trd>
  <trdgrp lng="pt">
   <trd>talco</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_l;&c_mol;&c_k;</trd>
  <trd lng="sv">talk</trd>
</drv>
</art>
