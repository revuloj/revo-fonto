<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>skandal</rad>/o
</kap>
<drv mrk="skanda.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Indigno ka&ubreve;zata de malbonekzemplaj agoj a&ubreve; paroloj:
      <ekz>
        <tld/>a konduto<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Beda&ubreve;rinda kaj dan&gcirc;era diskoniteco de iu malbona ekzemplo.
    </dif>
    <refgrp tip="sin">
      <ref cel="indign.0igajxo"></ref>,
      <ref cel="indign.0indajxo"></ref>
    </refgrp>
  </snc>
  <trd lng="de">Skandal</trd>
  <trdgrp lng="fr">
    <trd>scandale</trd>
  </trdgrp>
  <trd lng="nl">schandaal</trd>
</drv>

<drv mrk="skanda.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ka&ubreve;zi <tld/>on al iu.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>scandaliser</trd>
  </trdgrp>
  <trd lng="nl">te <ind>schande</ind> maken</trd>
</drv>
</art>
