<vortaro>
<art mrk="$Id$">
<kap>
  <rad>megafon</rad>/o
</kap>
<drv mrk="megafo.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Forta la&ubreve;tparolilo por reprodukti disa&ubreve;digojn en
      grandaj ejoj a&ubreve; sub
      libera &ccirc;ielo.
    </dif>
    <refgrp tip="vid">
      <ref cel="mikrof1.0o">mikrofono</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_e;&c_g;&c_a;&c_f;&c_o;&c_n;</trd>,
    <trd>&c_r;&c_u;&c_p;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="de">Megaphon</trd>
  <trd lng="en">megaphone</trd>
  <trd lng="es">meg&aacute;fono</trd>
  <trdgrp lng="hu">
    <trd>megafon</trd>,
    <trd>hangosbesz&eacute;l&odblac;</trd>
  </trdgrp>
  <trd lng="nl">megafoon</trd>
  <trdgrp lng="pl">
    <trd>megafon</trd>,
    <trd>szczekaczka<klr> (pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>megafone</trd>,
    <trd>alto-falante</trd>,
    <trd>porta-voz <klr tip="amb">(megafone)</klr></trd>    
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_g;&c_a;&c_f;&c_o;&c_n;</trd>,
    <trd>&c_r;&c_u;&c_p;&c_o;&c_r;</trd>
  </trdgrp>
  <trd lng="sk">megaf&oacute;n</trd>
</drv>
</art>
