<kap><rad>gastrotomi</rad>/o</kap>

<drv mrk="gastro4.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Operacio, dum kiu oni malfermas stomakon elekstere kaj celas la
      forigon de fremda korpo a&ubreve; tubero.
    </dif>
    <refgrp tip="vid">
      <ref cel="gastro3.0o">gastrostomio</ref>,
      <ref cel="laparo">laparotomio</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_s;&c_t;&c_r;&c_a;&c_s;&c_t;&c_a;&c_m;&c_ib;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>gastrotomie</trd>,
    <trd>&rcaron;ez b&rcaron;ichem</trd>
  </trdgrp>
  <trd lng="de">Gastrotomie</trd>
  <trd lng="el">&gamma;&alfa;&sigma;&tau;&rho;&omikron;&tau;&omikron;&my;&jota_ton;&alfa;</trd>
  <trd lng="fa">&f_g;&a_A;&a_s;&a_t;&a_r;&a_w;&a_t;&a_w;&a_m;&f_y;</trd>
  <trd lng="fr">gastrotomie</trd>
  <trdgrp lng="hu">
    <trd>gyomor m&udblac;t&eacute;ti megnyit&aacute;sa</trd>,
    <trd>gastrotomia</trd>
  </trdgrp>
  <trd lng="nl">gastrotomie</trd>
  <trd lng="pt">gastrotomia</trd>
  <trd lng="sk">oper&#xE1;cia &#x17E;al&#xFA;dka</trd>
  <trd lng="uk">&c_g;&c_a;&c_s;&c_t;&c_r;&#x43E;&c_t;&#x43E;&#x43C;&c_ib;&#x44F;</trd>
</drv>
</art>
