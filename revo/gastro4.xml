  <trd lng="fa">&f_g;&a_A;&a_s;&a_t;&a_r;&a_w;&a_t;&a_w;&a_m;&f_y;</trd>
  <trd lng="fr">gastrotomie</trd>
  <trdgrp lng="hu">
    <trd>gyomor m&udblac;t&eacute;ti megnyit&aacute;sa</trd>,
    <trd>gastrotomia</trd>
  </trdgrp>
  <trd lng="ja">&#x80C3;&#x5207;&#x958B;&#x8853;  <pr>&#x3044;&#x305B;&#x3063;&#x304B;&#x3044;&#x3058;&#x3085;&#x3064;</pr></trd>
  <trd lng="nl">gastrotomie</trd>
  <trd lng="pt">gastrotomia</trd>
  <trd lng="sk">oper&aacute;cia &zcaron;al&uacute;dka</trd>
  <trd lng="uk">&c_g;&c_a;&c_s;&c_t;&c_r;&c_o;&c_t;&c_o;&c_m;&c_ib;&c_ja;</trd>
