    <trd>&rcaron;ez b&rcaron;ichem</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gastrotomie</trd>,
    <trd>Magenschnitt</trd>
  </trdgrp>
  <trd lng="el">&gamma;&alfa;&sigma;&tau;&rho;&omikron;&tau;&omikron;&my;&jota_ton;&alfa;</trd>
