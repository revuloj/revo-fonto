<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>gastrotomi</rad>/o</kap>
<drv mrk="gastro4.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Operacio, dum kiu oni malfermas stomakon elekstere kaj celas la
      forigon de fremda korpo a&ubreve; tubero.
    </dif>
    <refgrp tip="vid">
      <ref cel="gastro3.0o">gastrostomio</ref>,
      <ref cel="laparo">laparotomio</ref>.
    </refgrp>
  </snc>
  <trd lng="el">&gamma;&alfa;&sigma;&tau;&rho;&omikron;&tau;&omikron;&my;&jota_ton;&alfa;</trd>
  <trd lng="fr">gastrotomie</trd>
  <trdgrp lng="hu">
    <trd>gyomor m&udblac;t&eacute;ti megnyit&aacute;sa</trd>,
    <trd>gastrotomia</trd>
  </trdgrp>
  <trd lng="nl">gastrotomie</trd>
  <trd lng="pt">gastrotomia</trd>
</drv>
</art>
