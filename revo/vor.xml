<vortaro>
<art mrk="$Id$">
<kap>
  <rad>vor</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="vor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <ref tip="dif" cel="mangx.0egi">man&gcirc;egi</ref>
    <ekz>
      <uzo tip="stl">FIG</uzo>
      la publiko <klr>...</klr> <tld/>is artikolojn kaj fotojn
      aperigitajn
      en kla&ccirc;revuoj
      <fnt><aut>Paul Gubbins</aut>: <vrk>Diana... &ccirc;u diino?</vrk>,
      <bib>Monato</bib>, <lok>jaro 1997a, numero 9a, p. 16a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_zh;&c_e;&c_r;&c_c;&c_ib;</trd>
  <trd lng="ca">devorar</trd>
  <trd lng="de">verschlingen</trd>
  <trd lng="es">devorar</trd>
  <trd lng="fr">d&eacute;vorer</trd>
  <trd lng="hu">fal</trd>
  <trdgrp lng="pl">
    <trd>&zdot;re&cacute;</trd>,
    <trd>po&zdot;era&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>devorar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_r;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_zh;&c_i;&c_r;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>

