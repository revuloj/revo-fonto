<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: gxibraltar.xml,v 1.4 2022/04/30 08:42:41 afido Exp $">
    <kap>
      <rad>&Gcirc;ibraltar</rad>/o, <var><kap><rad var="G">Gibraltar</rad>/o</kap></var>
    </kap>

<drv mrk="gxibraltar.0o">
  <kap><tld/>o, <var><kap><tld var="G"/>o</kap></var></kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Teritorio &ccirc;e la <ref cel="kol.mar0o">markolo</ref> liganta 
      <ref tip="malprt" cel="mediterane.M0o">Mediteraneo</ref>n kun Atlantiko:
      <ekz>
        la mistera maro <klr>[&#x2026;]</klr> etendas sin post la <tld var="G"/>a pordego
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        je la oka de oktobro ni traveturis la <tld/>an markolon
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        britaj transmaraj teritorioj <tld/>o kaj Montserato
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Steloj;monato2/007128.html">&Ccirc;u venos fino de la impostaj paradizoj?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en 1694 vraki&gcirc;is dum &scirc;tormo en Mediteraneo apud <tld/>o la brita milit&scirc;ipo <nom>Sussex</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>Elson B. Snow</aut>
          <vrk><url ref="&Monato;/2004/008155.html">Trez-oro submara</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_G;&c_ib;&c_b;&c_r;&c_a;&c_l;&c_t;&c_a;&c_r;</trd>
    <trd lng="de">Gibraltar</trd>
    <trd lng="en">Gibraltar</trd>
    <trd lng="fr">Gibraltar</trd>
  </snc>
  <trd lng="uk">&#x413;&#x456;&#x431;&#x440;&#x430;&#x43B;&#x442;&#x430;&#x440;</trd>
</drv>

  </art>

