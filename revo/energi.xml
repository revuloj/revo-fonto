<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>energi</rad>/o
</kap>
<drv mrk="energi.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Forto de la volo, stre&ccirc;ita al iu celo, kaj persista por &gcirc;in 
      atingi:
      <ekz>
        li &ccirc;iam montris grandan <tld/>on;
      </ekz>
      <ekz>
        <ind><tld/>dona</ind> man&gcirc;a&jcirc;o.
        <trd lng="fr">&eacute;nerg&eacute;tique</trd>
      </ekz>
    </dif>
  </snc>
  <snc mrk="energi.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Kapablo fari laboron:
      <ekz>
        la <ref cel="dimens.0o.FIZ">dimensioj</ref> de <tld/>o estas
        <frm>ml<sup>2</sup>t<sup>-2</sup></frm>;
      </ekz>
      <ekz>
        elektra, &hcirc;emia <tld/>o;
      </ekz>
      <ekz>
        varmo<tld/>o;
      </ekz>
      <ekz>
        muskola <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Energie</trd>
  <trd lng="en">energy</trd>
  <trd lng="fr">&eacute;nergie</trd>
  <trd lng="ru">&c_e;&c_n;&c_je;&c_r;&c_g;&c_i;&c_ja;</trd>
</drv>
<drv mrk="energi.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Posedanta <tld/>on:
      <ekz>
        <tld/>a karaktero, estro.
      </ekz>
    </dif>
    <trd lng="de">energetisch</trd>
  </snc>
  <snc>
    <dif>
      Forte impresanta, drasta 2:
      <ekz>
        <tld/>a esprimo, muziko.
      </ekz>
    </dif>
    <trd lng="de">energisch</trd>
  </snc>
  <trd lng="fr">&eacute;nergique</trd>
  <trd lng="ru">&c_e;&c_n;&c_je;&c_r;&c_g;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
</drv>
<drv mrk="energi.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      Kosmologia sistemo, en kiu oni konsideras la <tld/>on,
      kaj ne plu la materion kiel la substancon de la fizika mondo.
    </dif>
  </snc>
  <trd lng="fr">&eacute;nerg&eacute;tisme</trd>
  <trd lng="ru">&c_e;&c_n;&c_je;&c_r;&c_g;&c_je;&c_t;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
