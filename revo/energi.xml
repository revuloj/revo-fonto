<drv mrk="energi.akv0o">
  <kap>akv<tld/>o,
  <var><kap>akvo<tld/>o</kap></var></kap>
  <snc mrk="energi.akv0o.TEK">
    <uzo tip="fak">EKOL</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      <tld lit="E"/>o portata de naturaj akvomovoj a&ubreve; akvovarmeco,
      kaptebla per turbinoj a&ubreve; aliaj aparatoj:
      <ekz>
        akvo<tld/>o el ondoj de la maro a&ubreve; el riveroj
        <fnt>
          <aut>Asocio de Verduloj Esperantistaj</aut>,
          <vrk><url ref="http://www.verduloj.org/k_energi.htm">
          Efika Uzado de Energio</url></vrk>,
          <lok>2001</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Bara&jcirc;o Tri Gor&gcirc;oj estas akvo<tld/>a bara&jcirc;o en &Ccirc;inio,
        <klr>[&#x2026;]</klr>, la plej
        granda akv<tld/>a bara&jcirc;o el mondo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=34859">Akvobara&jcirc;o</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_d;&c_r;&c_a;&c_e;&c_n;&c_je;&c_r;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="en">hydropower</trd>
  <trd lng="fr">&eacute;nergie hydraulique</trd>
  <trd lng="pl">hydroenergia</trd>
</drv>

<drv mrk="energi.kern0o">
  <kap>kern<tld/>o,
  <var><kap>atom<tld/>o</kap></var>,
  <var><kap>nuklea <tld/>o</kap></var></kap>
  <snc mrk="energi.kern0o.TEK">
    <uzo tip="fak">EKOL</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      <tld lit="E"/>o eligata de atomkernoj, precipe dum
      ilia fendi&gcirc;o, uzata en nukleaj centraloj:
      <ekz>
        la procezo de fisio estis renkontita plurfoje dum la esploroj 
        pri kern<tld/>o dum la tridekaj jaroj de la 20-a jarcento
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Fisio">
          fisio</url>, 2011-01-16</lok>
        </fnt>;
      </ekz>
      <ekz>
        Germanujo estas 100 procente dependa de uranio-importoj
        por la atom<tld/>o
        <fnt>
          <aut>Bundesministerium f&uuml;r Umwelt, trad. F. Zeller</aut>,
          <vrk><url ref="http://www.verduloj.org/Atomenergio_erar-vojo.htm">
          Atomenergio: mal&gcirc;usta vojo</url></vrk>,
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi estas forta kontra&ubreve;ulo de nuklea <tld/>o
        <fnt>
          <aut>Benno</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/reza.torabi/88116">
          Profitoj kaj malprofitoj de la nuklea energio</url></vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_d;&c_z;&c_je;&c_r;&c_n;&c_a;&c_ja; &c_e;&c_n;&c_e;&c_r;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="en">nuclear power</trd>
  <trd lng="fr">&eacute;nergie nucl&eacute;aire</trd>
  <trdgrp lng="pl">
    <trd>energia j&aogonek;drowa</trd>,
    <trd>energia atomowa</trd>
  </trdgrp>
  <trd lng="uk">&#x430;&#x442;&#x43E;&#x43C;&#x43D;&#x430; &#x435;&#x43D;&#x435;&#x440;&#x433;&#x456;&#x44F;</trd>
</drv>

<drv mrk="energi.fosilia0o">
  <kap>fosilia <tld/>o</kap>
  <snc mrk="energi.fosilia0o.TEK">
    <uzo tip="fak">EKOL</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      <tld lit="E"/>o produktata per bruligo de
      fosiliaj substancoj: tergaso, karbo, naftoderiva&jcirc;oj:
      <ekz>
        tutmonda klimat&scirc;an&gcirc;i&gcirc;o da&ubreve;ras kaj fosilia <tld/>o ankora&ubreve; 
        el&ccirc;erpi&gcirc;as
        <fnt>
          <aut>M. Earle k.a.</aut>,
          <vrk><url ref="http://www.verduloj.org/Verda_Libro/VL01_klimato_energio.html">
          Verda Libro</url></vrk>,
          <lok>[2009]</lok>
        </fnt>;
      </ekz>
      <ekz>
        dikti la &ccirc;iam kreskantajn prezojn por fosilia energio
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="https://www.monato.net/2012/010595.php">Novtipa ekologia
          turismo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        oni klopodas redukti la importadon de fosiliaj <tld/>portantoj
        kaj pli uzi
        biomason, sun- kaj vent-energion kaj tervarmon
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="https://www.monato.net/2012/010555.php">Energiprezo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_n;&c_e;&c_r;&c_g;&c_ib;&c_ja; &c_z; &c_v;&c_y;&c_k;&c_a;&c_p;&c_n;&c_je;&c_v;&c_y;&c_h; &c_p;&c_a;&c_l;&c_ib;&c_w;</trd>
  <trd lng="fr">&eacute;nergie fossile</trd>
  <trdgrp lng="pl">
    <trd>energia kopalna <klr>(pot.)</klr></trd>,
    <trd>enrrgia z paliw kopalnych</trd>
  </trdgrp>
</drv>

<drv mrk="energi.renovigxanta0o">
  <kap>renovi&gcirc;anta <tld/>o, <var><kap>renovigebla
  <tld/>o</kap></var></kap>
  <snc mrk="energi.renovigxanta0o.TEK">
    <uzo tip="fak">EKOL</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      <tld lit="E"/>o provizata de fonto, kiun
      oni rigardas senlima, ne el&ccirc;erpi&gcirc;onta,
      kiel vento, suno... kontraste al
      <ref tip="ant" cel="energi.fosilia0o.TEK">fosilia <tld/>o</ref>:
      <ekz>
        le&gcirc;o pri renovigebla <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="https://www.monato.net/2012/010844.php">Sunelektraj kamparanoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;is 2030 E&ubreve;ropo havas devigajn celojn altigi
        la procenta&jcirc;on de renovigebla <tld/>o
        <fnt>
          <aut>D. ab Iago</aut>,
          <vrk><url ref="&Sezonoj;2013/03/bruselo-4/">
          Klimata &scirc;an&gcirc;i&gcirc;o post 2030</url></vrk>,
          <lok>La Balta Ondo, 2013-03-15</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_z;&c_n;&c_a;&c_w;&c_l;&c_ja;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_e;&c_n;&c_e;&c_r;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="en">renewable energy</trd>
  <trd lng="fr">&eacute;nergie renouvelable</trd>
  <trd lng="pl">energia odnawialna</trd>
</drv>

<drv mrk="energi.sun0o">
  <kap>sun<tld/>o</kap>
  <snc mrk="energi.sun0o.TEK">
    <uzo tip="fak">EKOL</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      <tld lit="E"/>o portata de sunradiado,
      kaptebla kaj reuzebla per diversaj aparatoj,
      interalie sun&ccirc;eloj:
      <ekz>
        paneloj por generi varmon a&ubreve; elektron el sun<tld/>o sur
        tegmentoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.net/2008/009613.php">Necesas energio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        fotoelektra sun<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Werner Fu&szlig;</aut>
          <vrk><url ref="https://www.monato.net/2011/010273.php">Verdigu la dezertojn!</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        sun<tld/>a forno <klr>[&#x2026;]</klr> ne bezonas brula&jcirc;on
        <fnt>
          <aut>E. Norman</aut>,
          <vrk><url ref="http://www.esperlando.com/sunenergio.htm">
          Suna Forno</url></vrk>,
          <lok>Esperlando, 2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_o;&c_n;&c_je;&c_ch;&c_n;&c_a;&c_ja; &c_e;&c_n;&c_e;&c_r;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="cs">slune&ccaron;n&iacute; energie</trd>
  <trd lng="en">solar energy</trd>
  <trd lng="fr">&eacute;nergie solaire</trd>
  <trdgrp lng="pl">
    <trd>energia s&lstroke;oneczna</trd>,
    <trd>energia solarna</trd>
  </trdgrp>
  <trd lng="sk">slne&ccaron;n&aacute; energia</trd>
  <trd lng="uk">&#x441;&#x43E;&#x43D;&#x44F;&#x447;&#x43D;&#x430; &#x435;&#x43D;&#x435;&#x440;&#x433;&#x456;&#x44F;</trd>
</drv>
 
<drv mrk="energi.tervarma0o">
  <kap>tervarma <tld/>o,
  <var><kap>geoterma <tld/>o</kap></var></kap>
  <snc mrk="energi.tervarma0o.TEK">
    <uzo tip="fak">EKOL</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      <tld lit="E"/>o el la internaj
      tavoloj de la tero, spontane disponebla
      en naturaj varmaj akvoj a&ubreve; kaptebla per diversaj subteraj
      ekipa&jcirc;oj:
      <ekz>
        nun, ventoenergio, sunenergio, tervarma <tld/>o <klr>[&#x2026;]</klr> 
        estas uzataj en &ccirc;iutaga vivo de tibet-naciaj kamparanoj
        <fnt>
          <vrk><url ref="http://www.espero.com.cn/old/epch/z06-06s/06s-2/07-06-2-16.htm">
          Lumigi Tibeton per nova fuelo</url></vrk>,
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        plej ofte oni produktas per la geoterma <tld/>o elektran 
        kurenton
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Geoterma_energio">
          Geoterma energio</url>, 2011-01-16</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_a;&c_t;&c_e;&c_r;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_e;&c_n;&c_e;&c_r;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="en">geothermal energy</trd>
  <trd lng="fr">&eacute;nergie g&eacute;othermique</trd>
  <trd lng="pl">energia geotermalna</trd>
</drv>

<drv mrk="energi.vent0o">
  <kap>vent<tld/>o, <var><kap>vento<tld/>o</kap></var></kap>
  <snc mrk="energi.vent0o.TEK">
    <uzo tip="fak">EKOL</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      <tld lit="E"/>o portata de vento, de aermovoj,
      kaptebla per veloj, ventoturbinoj kaj aliaj aparatoj:
      <ekz>
        motivi la elekton favore al vento<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Steloj;monato2/007052.html">Verda nature, science,
          socie</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Kalifornio produktas pli da vent<tld/>o
        ol iu ajn alia &scirc;tato en Usono
        <fnt>
          <aut>R. Miller</aut>,
          <vrk>Energio de Vento</vrk>,
          <lok><url ref="http://www.esperanto-chicago.org/newsletters/05_08.pdf">
          Saluton!, 2008-05</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_n;&c_e;&c_r;&c_g;&c_ib;&c_ja; &c_v;&c_je;&c_t;&c_r;&c_u;</trd>
  <trd lng="cs">energie v&ecaron;tru</trd>
  <trd lng="en">wind power</trd>
  <trd lng="fr">&eacute;nergie &eacute;olienne</trd>
  <trd lng="pl">energia wiatrowa</trd>
  <trd lng="sk">energia vetra</trd>
  <trd lng="uk">&#x435;&#x43D;&#x435;&#x440;&#x491;&#x456;&#x44F; &#x432;&#x456;&#x442;&#x440;&#x443;</trd>
</drv>
