<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: pit.xml,v 1.2 2022/05/14 20:32:21 afido Exp $">
    <kap>
      <rad>pit</rad>/a
    </kap>

<drv mrk="pit.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Morale, spirite <ref tip="super" cel="grand.mal0a.grado">malgranda</ref>:
      <ekz>
        li vidis la abismon en kiun li falis, la <klr>[&#8230;]</klr> malindajn dezirojn,
        vanajn esperojn, ruinajn kapablojn kaj <tld/>ajn motivojn, el kiuj konsistis
        lia ekzistado
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470; 11</lok>
        </fnt>;
      </ekz>
      <ekz>
        anstata&ubreve; tia idea diskuto, nun ni tamen atestas <tld/>ajn batalojn, rezignemon
        kaj malakordojn, kiuj <klr>[&#8230;]</klr> dis&scirc;prucis tra la tuta E-movado
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#8470; 8-9 (106-107)</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="anim.et0a">etanima</ref>
    <refgrp tip="vid">
      <ref cel="bagate.0ema">bagatelema</ref>,
      <ref cel="fend.har0i">harfendema</ref>,
      <ref cel="nap.0okapa">napokapa</ref>,
      <ref cel="pedant.0a.ekzaktema">pedanta<sncref/></ref>
    </refgrp>
    <trd lng="de">kleingeistig</trd>
    <trd lng="en">petty</trd>
    <trd lng="fr">petit <klr>(esprit)</klr></trd>
    <trd lng="pl">ma&lstroke;y  <klr>(pogardliwie)</klr></trd>
    <trd lng="ro">mic  <klr>(dispre&#539;uitor)</klr></trd>
  </snc>
</drv>

  </art>

