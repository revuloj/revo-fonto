<art mrk="$Id$">
<kap>
  <rad>hibisk</rad>/o
</kap>
<drv mrk="hibisk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="malvac.0oj">malvacoj</ref>
      (<trd lng="la">Hibiscus</trd>), rimarkinda pro siaj grandaj,
      brilkoloraj floroj.
    </dif>
  </snc>
  <bld lok="../bld/hibisk.jpg">
    floro de <tld/>o
  </bld>
  <trd lng="de">Hibiskus</trd>
  <trd lng="fr">hibiscus</trd>
  <trdgrp lng="hu">
    <trd>m&aacute;lyvacserje</trd>,
    <trd>hibiszkusz</trd>
  </trdgrp>
  <trd lng="nl">hibiscus</trd>
  <trdgrp lng="pl">
    <trd>hibiskus</trd>, <trd>ketmia</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_i;&c_b;&c_i;&c_s;&c_k;&c_u;&c_s;</trd>
</drv>
</art>
