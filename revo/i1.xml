<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: i1.xml,v 1.9 2024/04/03 06:28:35 revo Exp $">
<kap>
    <ofc>*</ofc>-<rad>i</rad>
</kap>

<drv mrk="i1.0">
  <kap>-<tld/></kap>
  <snc mrk="i1.0.finajxo">
    <uzo tip="fak">GRA</uzo>
    <dif>
      <ref tip="super" cel="fin.0ajxo.GRA">Fina&jcirc;o</ref> markanta
      la <ref cel="infini2.0o">infinitivon</ref> &ccirc;e
      <ref cel="verb.0o">verbo</ref>:
      <ekz>
        dum ridado povas dolor<tld/> la koro
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 14:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Kristaneco ordonas al ni pardon<tld/> al niaj malamikoj, am<tld/> nian
        proksimulon
        <fnt>
          <bib>Fab3</bib>
          <lok>Juda knabino</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u hundo a&ubreve; &ccirc;evalo <klr>[&#8230;]</klr> &dash; plor<tld/> ili povas, sed rid<tld/> &dash; tiun
        kapablon havas nur la homo
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        granda feli&ccirc;o estas am<tld/> <klr>[&scirc;in]</klr>
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> ludis inspirite sur harpo, por la&ubreve;d<tld/> kaj glor<tld/> la
        Eternulon
        <fnt>
          <bib>MT</bib>
          <lok>I. Kroniko 25:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> alvenis al <nom>Jordan</nom> <klr>[&#8230;]</klr> kaj ili tradormis tie la
        nokton, anta&ubreve; ol
        transir<tld/>
        <fnt>
          <bib>MT</bib>
          <lok>Josuo 3:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        la princo anstata&ubreve; rigard<tld/> saltis sur la vojeton kaj baris la vojon
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="as1.0">-as</ref>,
      <ref cel="is.0">-is</ref>,
      <ref cel="os.0">-os</ref>,
      <ref cel="us.0">-us</ref>,
      <ref cel="u1.0">-u</ref>
    </refgrp>
    <trd lng="de">-en <klr>(Infinitiv)</klr></trd>
  </snc>
  <refgrp tip="hom">
    <ref cel="i.0.sufikso">-<tld/>/<klr> (&scirc;ajna sufikso)</klr></ref>
  </refgrp>
  <trd lng="be">&c_a;&c_f;&c_ib;&c_k;&c_s; &c_n;&c_je;&c_a;&c_z;&c_n;&c_a;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_j; &c_f;&c_o;&c_r;&c_m;&c_y; &c_d;&c_z;&c_je;&c_ja;&c_s;&c_l;&c_o;&c_v;&c_a; &c_w; &c_e;&c_s;&c_p;&c_e;&c_r;&c_a;&c_n;&c_t;&c_a;</trd>
  <trd lng="fr"><klr>(infinitif de verbes)</klr></trd>
  <trdgrp lng="io">
    <trd>-ir<klr> (estinteco)</klr></trd>,
    <trd>-ar<klr> (estanteco)</klr></trd>,
    <trd>-or<klr> (estonteco)</klr></trd>
  </trdgrp>
  <trd lng="vo">-&ouml;n</trd>
</drv>
</art>

