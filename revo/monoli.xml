<drv mrk="monoli.0o">
  <kap><tld/>o</kap>
  <snc mrk="monoli.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>    
    <dif>
      Laboro farita nur el <ref cel="unu.0">unu</ref> &scirc;tona <ref cel="blok.0o.maso">bloko</ref>:
      <ekz>
        <ref cel="obelis.0o">obelisko</ref>j origine estis <tld/>oj;
      </ekz>
      <ekz>
        la marmora <tld/>o generinta la faman skulpta&jcirc;on estis &ccirc;irka&ubreve;&ccirc;izita
        <fnt>
          <vrk><url ref="https://www.museoomero.it/eo/opere/davido-de-michelangelo/">Davido de Michelangelo - Museo Omero</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kiam la &scirc;tataj <tld/>oj estis dispecigitaj kaj disvenditaj al pli rendimentaj
        privataj kompanioj, floris la sektoroj komunikaj, gasaj, elektraj
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2013/011033.php">Forpasis la (in)fera sinjorino</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/1/1c/Templo_de_Tihuanacu_-_L-00-31_croped.jpg/479px-Templo_de_Tihuanacu_-_L-00-31_croped.jpg" prm="CC BY-SA 4.0"><tld/>o de Bennett, Tiwanako, Bolivio
        <fnt>
          <aut>P. Beaudroit</aut>
          <lok><url ref="&WCW;/File:Templo_de_Tihuanacu_-_L-00-31_croped.jpg">&WCW;/File:Templo_de_Tihuanacu_-_L-00-31_croped.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <ref tip="vid" cel="ejnsxtejn.0o">Ejn&scirc;tejno</ref>
  <trd lng="be">&c_m;&c_a;&c_n;&c_a;&c_l;&c_ib;&c_t;</trd>
