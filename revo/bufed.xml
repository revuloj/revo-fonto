<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>bufed</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="bufed.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bufed.0o.MEBL">
    <dif>
      <ref tip="lst" lst="voko:mebloj" cel="mebl.0o">Meblo</ref>
      provizita per man&gcirc;a&jcirc;oj disponeblaj por invititoj
      a&ubreve; klientoj &ccirc;e teatro, balo, ina&ubreve;guro ktp.:
      <ekz>
        li donis inter&scirc;an&gcirc;e la kokinon, ricevis la pomoj
        kaj eniris en la drinkejon rekte al la <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Kion la patro faras, estas &ccirc;iam &gcirc;usta</lok>
        </fnt>.
      </ekz>
    </dif>   
    <trd lng="br">taoliad veuzio&ugrave;</trd>
    <trdgrp lng="de">
      <trd>B&uuml;ffet</trd>,
      <trd>Anrichte</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>b&uuml;f&eacute;</trd>,
      <trd>t&aacute;lal&oacute;asztal</trd>
    </trdgrp>
    <trd lng="nl">buffet</trd>
    <trdgrp lng="it">
      <trd>buff&egrave;</trd>,
      <trd>buffet</trd>,
      <trd>banco<klr> (di bar)</klr></trd>,
      <trd>bancone<klr> (di bar)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>bufet</trd>,
      <trd>lada</trd>,
      <trd>bar</trd>,
      <trd>kredens</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>aparador</trd>,
      <trd>bif&ecirc;</trd>
    </trdgrp>
  </snc>
  <snc mrk="bufed.0o.ejo">
    <ref tip="dif" cel="bufed.0ejo.KUI"><tld/>ejo</ref>
    <ekz>
      en <tld/>o sidis maljunulino
      <fnt>
        <aut>J. Vajs</aut>,
        <vrk><url
        ref="http://www.poludnica.esperanto.sk/index.php?akcia=info&amp;kategorio=8&amp;co=35">
        Musala</url></vrk>,
        <lok>[2014?]</lok>
      </fnt>.
    </ekz>
    <trd lng="de">Lounge</trd>
    <trd lng="nl">buffet</trd>
    <trdgrp lng="pl">
      <trd>bufet</trd>,
      <trd>bar</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_f;&c_e;&c_t;</trd>
  <trd lng="ca">bufet</trd>
  <trdgrp lng="cs">
    <trd>bufet</trd>,
    <trd>st&uring;l s ob&ccaron;erstven&iacute;m</trd>
  </trdgrp>
  <trd lng="en">buffet</trd>
  <trd lng="es">buffet</trd>
  <trd lng="fr">buffet</trd>
  <trd lng="hu">b&uuml;f&eacute;</trd>
  <trdgrp lng="it">
    <trd>buvette</trd>,
    <trd>buff&egrave;<klr> (luogo dove...)</klr></trd>,
    <trd>buffet<klr> (luogo dove...)</klr></trd>, 
    <trd>caff&egrave;<klr tip="amb"> (luogo di ristoro)</klr></trd>,
    <trd>bar</trd>
  </trdgrp>
  <trd lng="ro">bufet</trd>
  <trd lng="ru">&c_b;&c_u;&c_f;&c_je;&c_t;</trd>
  <trd lng="sk">bufet</trd>
  <trd lng="sv">buff&eacute;</trd>
</drv>

<drv mrk="bufed.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="bufed.0ejo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="mangx.0ejo.KUI">Man&gcirc;ejo</ref>,
      kie funkcias
      <ref tip="vid" cel="bufed.0o.MEBL"><tld/>o</ref>:
      <ekz>
        li na&ubreve;ze fintrinkis lastan kafgluton kaj foriris el la <tld/>ejo
        <fnt>
          <bib>Monato</bib>
          <vrk><url
          ref='http://www.esperanto.be/fel/mon/006805.html'>Kota Akvo:
          Konstanten Krysakov</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">buffet</trd>
</drv>
</art>
