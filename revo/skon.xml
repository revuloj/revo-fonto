<art mrk="$Id: skon.xml,v 1.7 2024/03/25 06:40:50 afido Exp $">
<kap>
  <rad>skon</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>

<drv mrk="skon.0o">
  <kap><tld/>o</kap>
  <snc mrk="skon.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
       Skotdevena, delikata paneto kutime ronda, ordinare man&gcirc;ata kun butero en pluraj angla-lingvaj
       landoj:
       <ekz>
        la gnomoj &jcirc;us ekman&gcirc;is la buteritajn <tld/>ojn, kiam a&ubreve;di&gcirc;is la&ubreve;ta frapo
        <fnt>
          <aut>J. R. R. Tolkien, trad. C. Gledhill</aut>
          <vrk>La Hobito</vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        en Usono, <tld/>oj estas pli sekaj kaj pli grandaj, kaj tipe dol&ccirc;aj
        <fnt>
          <vrk><url ref="&Viki;/Skono">Skono - Vikipedio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="bulk.0o">bulko</ref>
    <bld lok="&WCU;/thumb/9/96/Fresh_cranberry_scones_03.png/320px-Fresh_cranberry_scones_03.png" prm="CC BY-SA 4.0"><tld/>oj kun oksikokoj kaj surmetita butero
        <fnt>
          <aut>Infrogmation, propra verko</aut>
          <lok><url ref="&WCW;/File:Fresh_cranberry_scones_03.png">&WCW;/File:Fresh_cranberry_scones_03.png</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="de">Scone</trd>
  <trd lng="en">scone</trd>
  <trd lng="fr">scone</trd>
  <trd lng="pl">scone  <klr>(ciastko szkockie)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x44F;&c_ch;&#x43C;&c_ib;&#x43D;&#x43D;&c_i;&c_j; &c_a;&c_b;&#x43E; &#x43F;&c_sh;&c_je;&#x43D;&c_i;&c_ch;&#x43D;&c_i;&c_j;
    &#x43A;&#x43E;&c_r;&c_zh;</trd>,
    <trd>&c_b;&c_u;&#x43B;&#x43E;&c_ch;&#x43A;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x70E4;&#x9905; <pr>k&#x1CE;ob&#x1D0;ng</pr></trd>,
    <trd>&#x70E4;&#x997C; <pr>k&#x1CE;ob&#x1D0;ng</pr></trd>
  </trdgrp>
</drv>
