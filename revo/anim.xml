<drv mrk="anim.et0a">
  <kap>et<tld/>a</kap>
  <snc mrk="anim.et0a.PSI">
    <dif>
      Malnobla, malbonhumora, avara, speciale pri bagateloj.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>bez du&scaron;evn&iacute; s&iacute;ly</trd>,
    <trd>malomysln&yacute;</trd>
  </trdgrp>
  <trd lng="de">kleingeistig</trd>
  <trdgrp lng="en">
    <trd>small-<ind>mind</ind>ed</trd>,
    <trd>petty</trd>
  </trdgrp>
  <trd lng="he">&qof;&tet;&nun;&vav;&nun;&yod;</trd>
  <trd lng="it">meschino</trd>
  <trdgrp lng="pl">
    <trd>ma&lstroke;ostkowy</trd>,
    <trd>ma&lstroke;oduszny</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>m&abreve;runte</trd>,
    <trd>minore</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_je;&c_l;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">malomyse&lcaron;n&yacute;</trd>
</drv>

<drv mrk="anim.et0eco">
  <kap>et<tld/>eco</kap>
  <snc mrk="anim.et0eco.PSI">
    <dif>
      Emo agi avare kaj mal&gcirc;entile, speciale pri bagateloj.
    </dif>
  </snc>
  <trd lng="cs">malost</trd>
  <trdgrp lng="en">
    <trd>small-<ind>mind</ind>edness</trd>,
    <trd>pettiness</trd>
  </trdgrp>
  <trd lng="he">&qof;&tet;&nun;&vav;&nun;&yod;&vav;&tav;</trd>
  <trd lng="it">meschinit&agrave;</trd>
  <trdgrp lng="pl">
    <trd>ma&lstroke;ostkowo&sacute;&cacute;</trd>,
    <trd>ma&lstroke;oduszno&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>m&acirc;ng&acirc;i</trd>,
    <trd>mangaiere</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_je;&c_l;&c_o;&c_ch;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">malos&tcaron;</trd>
</drv>

<drv mrk="anim.facil0a">
  <kap>facil<tld/>a</kap>
  <snc mrk="anim.facil0a.PSI">
    <dif>
      Senpripensa, senkonsidera, neserioza:
      <ekz>
        la virinoj estas tiel facil<tld/>aj
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_je;&c_g;&c_k;&c_a;&c_d;&c_u;&c_m;&c_n;&c_y;</trd>
  <trd lng="cs">lehkomysln&yacute;</trd>
  <trd lng="de">leichtfertig</trd>
  <trdgrp lng="en">
    <trd>light-<ind>mind</ind>ed</trd>,
    <trd>frivolous</trd>,
    <trd>flippant</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>insouciant</trd>,
    <trd>l&eacute;ger<klr tip="ind"> (insouciant)</klr></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&qof;&lamed; &dalet;&ayin;&tav;</trd>,
    <trd>&pe;&zayin;&yod;&zayin;</trd>
  </trdgrp>
  <trd lng="hu">k&ouml;nnyelm&udblac;</trd>
  <trdgrp lng="it">
    <trd>allegro<klr> (frivolo)</klr></trd>,
    <trd>leggero<klr> (frivolo)</klr></trd>,
    <trd>spensierato</trd>,
    <trd>avventato</trd>
  </trdgrp>
  <trd lng="nl">ondoordacht</trd>
  <trdgrp lng="pl">
    <trd>roztargniony</trd>,
    <trd>frywolny</trd>,
    <trd>niepowa&zdot;ny</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>distras</trd>,
    <trd>frivol</trd>,
    <trd>frivole</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_je;&c_g;&c_k;&c_o;&c_m;&c_y;&c_s;&c_l;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">&lcaron;ahkomyse&lcaron;n&yacute;</trd>
</drv>

<drv mrk="anim.facil0ajxo">
  <kap>facil<tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Malserioza, senpripensa ago:
      <ekz>
        &ccirc;u oni povas tiel konstante faradi facil<tld/>a&jcirc;ojn
        <fnt><bib>Marta</bib></fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="en">frivolity</trd>
  <trdgrp lng="fr">
    <trd>b&ecirc;tise<klr tip="amb"> (acte irr&eacute;fl&eacute;chi)</klr></trd>,
    <trd>folie<klr tip="amb"> (acte irr&eacute;fl&eacute;chi)</klr></trd>
  </trdgrp>
  <trd lng="he">&shin;&tet;&vav;&tav;</trd>
   <trdgrp lng="it">
    <trd>faciloneria<klr> (atto)</klr></trd>,
    <trd>spensieratezza<klr> (atto)</klr></trd>,
    <trd>avventatatezza<klr> (atto)</klr></trd>
  </trdgrp>
  <trd lng="pl">lekkomy&sacute;lno&sacute;&cacute;</trd>
  <trd lng="ro">nep&abreve;sare</trd>
  <trd lng="ru">&c_l;&c_je;&c_g;&c_k;&c_o;&c_m;&c_y;&c_s;&c_l;&c_je;&c_n;&c_n;&c_y;&c_j; &c_p;&c_o;&c_s;&c_t;&c_u;&c_p;&c_o;&c_k;</trd>
</drv>

<drv mrk="anim.facil0eco">
  <kap>facil<tld/>eco</kap>
  <snc mrk="anim.facil0eco.PSI">
    <dif>
      Malserioza humoro a&ubreve; karaktero, emo al agado sen kalkulo de
      sekvoj:
      <ekz>
        mia <klr>[&#8230;]</klr> koro malsa&gcirc;e pu&scirc;is min en &ccirc;i tiun
        facil<tld/>econ
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 9a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">lehkomyslnost</trd>
  <trdgrp lng="en">
    <trd>light-<ind>mind</ind>edness</trd>,
    <trd>frivolity</trd>,
    <trd>flippancy</trd>
  </trdgrp>
  <trd lng="fr">l&eacute;g&egrave;ret&eacute;<klr tip="amb"> (manque de
  s&eacute;rieux)</klr></trd>
  <trdgrp lng="he">
    <trd>&qof;&lamed;&vav;&tav; &dalet;&ayin;&tav;</trd>,
    <trd>&pe;&zayin;&yod;&zayin;&vav;&tav;</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>faciloneria</trd>,
    <trd>spensieratezza</trd>,
    <trd>avventatatezza</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>lekkomy&sacute;lno&sacute;&cacute;</trd>,
    <trd>beztroska</trd>,
    <trd>charakterek</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>nep&abreve;sare</trd>,
    <trd>frivolitate</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_je;&c_g;&c_k;&c_o;&c_m;&c_y;&c_s;&c_l;&c_i;&c_je;</trd>,
    <trd>&c_v;&c_je;&c_t;&c_r;&c_je;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_je;&c_r;&c_mol;&c_jo;&c_z;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">&lcaron;ahkomyse&lcaron;nos&tcaron;</trd>
</drv>

<drv mrk="anim.grand0a">
  <kap><ofc>*</ofc> grand<tld/>a <fnt><bib>F</bib>, <lok>Universala
  Vortaro, <url ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5039">grand'anim'</url></lok></fnt></kap>
  <snc mrk="anim.grand0a.PSI">
    <dif>
      <ref tip="super" cel="nobl.0a">Nobla</ref>, donema, pardonema,
      senenvia:
      <ekz>
        &ccirc;u ne konvenas al la venkinto esti grand<tld/>a
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 13a</lok></fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_je;&c_l;&c_ib;&c_k;&c_a;&c_d;&c_u;&c_sh;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>velkodu&scaron;n&yacute;</trd>,
    <trd>velkomysln&yacute;</trd>,
    <trd>velkorys&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gro&szlig;herzig</trd>,
    <trd>gro&szlig;z&uuml;gig</trd>
  </trdgrp>
  <trd lng="en">magnanimous</trd>
  <trd lng="fr">magnanime</trd>
  <trdgrp lng="he">
    <trd>&nun;&dalet;&yod;&bet;</trd>,
    <trd>&alef;&tsadi;&yod;&lamed; &nun;&pe;&shin;</trd>
  </trdgrp>
  <trd lng="it">magnanimo</trd>
  <trd lng="nl">groothartig</trd>
  <trd lng="pl">wielkoduszny</trd>
  <trd lng="ro">m&abreve;rinimos</trd>
  <trd lng="ru">&c_v;&c_je;&c_l;&c_i;&c_k;&c_o;&c_d;&c_u;&c_sh;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">ve&lcaron;kodu&scaron;n&yacute;</trd>
</drv>

<drv mrk="anim.grand0eco">
  <kap>grand<tld/>eco</kap>
  <snc mrk="anim.grand0eco.PSI">
    <dif>
      Kapablo agi noble, pardoneme sen postkolero a&ubreve; malfido:
      <ekz>
        Asirianoj, malgra&ubreve; la barbareco, ne estas malbonaj homoj, se
        ili
        &scirc;atas la grand<tld/>econ
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 11a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">velkomyslnost</trd>
  <trd lng="en">magnanimity</trd>
  <trdgrp lng="fr">
    <trd>largeur d'esprit</trd>,
    <trd>magnanimit&eacute;</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&nun;&dalet;&yod;&bet;&vav;&tav;</trd>,
    <trd>&resh;&vav;&het;&bet; &lamed;&bet;</trd>,
    <trd>&alef;&tsadi;&yod;&lamed;&vav;&tav; &nun;&pe;&shin;</trd>,
    <trd>&gimel;&dalet;&lamed;&vav;&tav; &nun;&pe;&shin;</trd>
  </trdgrp>
  <trd lng="it">magnanimit&agrave;</trd>
  <trd lng="pl">wielkoduszno&sacute;&cacute;</trd>
  <trd lng="ro">generozitate</trd>
  <trd lng="ru">&c_v;&c_je;&c_l;&c_i;&c_k;&c_o;&c_d;&c_u;&c_sh;&c_i;&c_je;</trd>
  <trd lng="sk">&scaron;&lcaron;achetnos&tcaron;</trd>
</drv>

<drv mrk="anim.kot0ulo">
  <kap>kot<tld/>ulo</kap>
  <snc>
    <dif>
      <ref tip="super" cel="nobl.mal0ulo">Malnoblulo</ref> kun trivialaj
      pensoj, nekapabla de bonaj agoj:
      <ekz>
        fi al la abomeninda kot<tld/>ulo, kiu &scirc;in trompe forlasis
        <fnt><bib>Fab4</bib>, <lok>Sterkskarabo</lok></fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="en">low-<ind>mind</ind>ed person</trd>
  <trd lng="fr">ignoble individu</trd>
  <trd lng="he"><ind>&gimel;&samekh;</ind> &resh;&vav;&het;</trd>
  <trd lng="it">ignobile<klr> (individuo)</klr></trd>
  <trd lng="pl">pod&lstroke;y cz&lstroke;owiek</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_l;&c_je;&c_c;</trd>,
    <trd>&c_p;&c_o;&c_sh;&c_l;&c_ja;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="anim.largx0a">
  <kap>lar&gcirc;<tld/>a</kap>
  <snc mrk="anim.largx0a.PSI">
    <dif>
      Volonte toleranta diversajn opiniojn, akceptanta diversajn
      vidpunktojn:
      <ekz>
        li estas lar&gcirc;<tld/>a liberpensulo
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 3a, Lemuel da&ubreve;rigas sian
        esploron</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="toler.0ema">tolerema</ref>
    </refgrp>
  </snc>
  <trd lng="en">open-<ind>mind</ind>ed</trd>
  <trd lng="fr">large d'esprit</trd>
  <trd lng="he">&resh;&het;&bet; &alef;&vav;&pe;&qof;</trd>
  <trd lng="pl">otwarty</trd>
  <trd lng="ro">deschide</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_i;&c_r;&c_o;&c_k;&c_i;&c_h; &c_v;&c_z;&c_g;&c_l;&c_ja;&c_d;&c_o;&c_v;</trd>,
    <trd>&c_b;&c_je;&c_z; <ind>&c_p;&c_r;&c_je;&c_d;&c_u;&c_b;&c_je;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_j;</ind></trd>,
    <trd>&c_n;&c_je;&c_p;&c_r;&c_je;&c_d;&c_v;&c_z;&c_ja;&c_t;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="anim.largx0eco">
  <kap>lar&gcirc;<tld/>eco</kap>
  <snc mrk="anim.largx0eco.PSI">
    <dif>
      Akceptanta &ccirc;iajn doktrinojn a&ubreve; sintenojn &ccirc;e aliaj homoj, pli
      volonte
      vidanta iliajn akordojn ol iliajn malakordojn.
    </dif>
  </snc>
  <trd lng="en">open-<ind>mind</ind>edness</trd>
  <trd lng="fr">largeur d'esprit</trd>
  <trd lng="he">&resh;&vav;&het;&bet; &alef;&vav;&pe;&qof;</trd>
  <trd lng="pl">otwarto&sacute;&cacute;</trd>
  <trd lng="ro">deschidere</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_i;&c_r;&c_o;&c_t;&c_a; &c_v;&c_z;&c_g;&c_l;&c_ja;&c_d;&c_o;&c_v;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_r;&c_je;&c_d;&c_u;&c_b;&c_je;&c_zh;&c_d;&c_jo;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_r;&c_je;&c_d;&c_v;&c_z;&c_ja;&c_t;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="anim.pur0a">
  <kap>pur<tld/>a</kap>
  <snc mrk="anim.pur0a.PSI">
    <dif>
      Morale sendifekta, senartifika kaj bonvola:
      <ekz>
        mi devas esti pur<tld/>a e&ccirc; en laika mondo kvaza&ubreve; la lotusa 
        floro estas pura e&ccirc; en kota lago
        <fnt>
          <aut>-, trad. Ni&scirc;i Jasuhiro</aut>,
          <vrk><url ref="http://esperanto.us/Budhanaj/42Cxapitroj.html">
          La 42 &Ccirc;apitroj</url></vrk>,
          <lok>La Japana Budhano, 1999 (304-306)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">candide</trd>
</drv>
   
<drv mrk="anim.rekt0a">
  <kap>rekt<tld/>a</kap>
  <snc mrk="anim.rekt0a.PSI">
    <dif>
      Senartifika, ne devoji&gcirc;ema, konservanta sian celon:
      <ekz>
        la rekt<tld/>a vivmaniero de la tiamaj homoj
        vekas emocion en ni
        <fnt>
          <aut>Vastalto</aut>,
          <vrk><url ref="http://www.vastalto.com/blog">Akvosfero</url></vrk>,
          <lok>2006-03-11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>droit<klr> (moralement)</klr></trd>,
    <trd>probe</trd>
  </trdgrp>
</drv>

<drv mrk="anim.simpl0a">
  <kap>simpl<tld/>a</kap>
  <snc mrk="anim.simpl0a.PSI">
    <dif>
      Naive, senartifike sincera:
      <ekz>
        Olnjo kaj la pastro longe ridis simpl<tld/>e
        je tiu &ccirc;i neatendita falo
        <fnt><aut>I. G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>, 
        <lok>[1898?]</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">prostoduch&yacute;</trd>
  <trd lng="fr">ing&eacute;nu</trd>
  <trd lng="sk">prostoduch&yacute;</trd>
</drv>

<drv mrk="anim.unu0e">
  <kap>unu<tld/>e</kap>
  <snc>
    <dif>
      &Ccirc;iukonsente.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_n;&c_a;&c_d;&c_u;&c_sh;&c_n;&c_a;</trd>
  <trd lng="ca">un&agrave;nimement</trd>
  <trd lng="de">einstimmig</trd>
  <trd lng="en">unanimously</trd>
  <trd lng="es">un&aacute;nimemente</trd>
  <trdgrp lng="fr">
    <trd>unanimement</trd>,
    <trd>&agrave; l'<ind>unanimit&eacute;</ind></trd>
  </trdgrp>
  <trd lng="he"><ind>&pe;&he;</ind>-&alef;&het;&dalet;</trd>
  <trd lng="hu">egyhang&uacute;lag</trd>
  <trd lng="it">all'<ind>unanimit&agrave;</ind></trd>
  <trd lng="nl">eensgezind</trd>
  <trd lng="pl">jednomy&sacute;lnie</trd>
  <trd lng="pt">unanimemente</trd>
  <trd lng="ro">&icirc;n unanimitate</trd>
  <trd lng="ru">&c_je;&c_d;&c_i;&c_n;&c_o;&c_d;&c_u;&c_sh;&c_n;&c_o;</trd>
  <trd lng="sk">jednomyse&lcaron;ne</trd>
</drv>
