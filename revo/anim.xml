<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>anim</rad>/o
</kap>
<drv mrk="anim.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="anim.0o.vivo">
    <uzo tip="fak">FIL</uzo>
    <uzo tip="fak">REL</uzo>
    <dif>
      Spirita, viviga principo de homo, la&ubreve; iuj doktrinoj eterna:
      <ekz>
        la korpo estas morta, la <tld/>o estas senmorta
        <fnt><bib>F</bib>, <lok>&FE; 31</lok></fnt>;
      </ekz>
      <ekz>
        oni ofte kontra&ubreve;metas
        la nemateriecon de la <tld/>o al la materieco de la korpo;
      </ekz>
      <ekz>
        eligi sian <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        pre&gcirc;i por la <tld/>o de iu;
      </ekz>
      <ekz>
        vagi kiel turmentata <tld/>o.
      </ekz>
      <ekz>
        fordoni sian <tld/>on al la diablo;
      </ekz>
      <ekz>
        lia <tld/>o forkuris en la pinton de la piedo
        <klr>(li timegi&gcirc;is
        <refgrp tip="vid">
          <ref cel="trem.0i">tremi</ref>,
          <ref cel="hauxt.0o">ha&ubreve;to</ref>
        </refgrp>)</klr><fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ili vivas per unu <tld/>o en du korpoj
        <refgrp tip="vid">
          <ref cel="intim.0a">intima</ref>,
          <ref cel="fid.0o">fido</ref>
        </refgrp>
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
  </snc>
  <snc mrk="anim.0o.sentaro">
    <dif>
      Senta kaj pasia parto de la <ref tip="malprt"
      cel="mens.0o">menso</ref>, tuto de la moralaj ecoj, bonaj a&ubreve;
      malbonaj, kontraste al la intelekto:
      <ekz>
        nobla <tld/>o;
      </ekz>
      <ekz>
        sonoj ne a&ubreve;deblaj, sed senteblaj por &ccirc;iu
        <tld/>o sentema<fnt>Z</fnt>;
      </ekz>
      <ekz>
        staras anta&ubreve; miaj <tld/>aj okuloj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kanti per sia tuta <tld/>o <klr>(fervorege)</klr>;
      </ekz>
      <ekz>
        Tolstoj estis pli &gcirc;uste granda <tld/>o ol granda intelekto.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kor.0o.FIG">koro <sncref/></ref>
    </refgrp>
  </snc>
  <snc mrk="anim.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      &Ccirc;efinstiganto:
      <ekz>
        li estas la <tld/>o de la ribelo, de la entrepreno<fnt>B</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kern.0o">kerno</ref>,
      <ref cel="motor.0o">motoro</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_u;&c_sh;&c_a;</trd>,
    <trd>&c_d;&c_u;&c_h;</trd>
  </trdgrp>
  <trd lng="ca">&agrave;nima</trd>
  <trd lng="cs">du&scaron;e</trd>
  <trd lng="de">Seele</trd>
  <trd lng="es">alma</trd>
  <trd lng="fr">&acirc;me</trd>
  <trd lng="hu">l&eacute;lek</trd>
  <trd lng="it">anima</trd>
  <trd lng="nl">ziel</trd>
  <trd lng="ru">&c_d;&c_u;&c_sh;&c_a;</trd>
  <trd lng="sk">du&scaron;a</trd>
</drv>

<drv mrk="anim.0i">
  <kap><tld/>i<fnt>Z</fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Vivigi:
      <ekz>
        <tld/>ita esta&jcirc;o;
      </ekz>
      <ekz>
        trovi la &gcirc;ustan vojon al <tld/>ado de la lingvo.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_d;&c_u;&c_sh;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>
    <trd lng="ca">animar</trd>
    <trd lng="hu">lelket lehel bele</trd>
    <trd lng="it">animare</trd>
    <trd lng="nl">bezielen</trd>
    <trd lng="ru">&c_o;&c_d;&c_u;&c_sh;&c_je;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Vigligi:
      <ekz>
        <tld/>i la enuan atmosferon de oficiala kunveno;
      </ekz>
      <ekz>
        <tld/>ita parolado<fnt>K</fnt>,
        <trd lng="hu">lelkes besz&eacute;d</trd>
      </ekz>
      <ekz>
        diskuto;
      </ekz>
      <ekz>
        <tld/>itaj komentarioj.
        <trd lng="hu">lelkes&uuml;lt komment&aacute;torok</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_zh;&c_y;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>
    <trd lng="hu">lelkes&iacute;t</trd>
    <trd lng="nl">animeren</trd>
    <trd lng="ru">&c_o;&c_zh;&c_i;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Inspiri:
      <ekz>
        poemo <tld/>ita de malamo al la tiraneco;
      </ekz>
      <ekz>
        <tld/>itaj de &ccirc;i tiu spirito ni proponis...
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_t;&c_h;&c_n;&c_ja;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_d;&c_u;&c_h;&c_a;&c_t;&c_v;&c_a;&c_r;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="nl">inspireren</trd>
    <trd lng="ru">&c_v;&c_o;&c_o;&c_d;&c_u;&c_sh;&c_je;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="de">
    <trd>beseelen</trd>,
    <trd>beleben</trd>
  </trdgrp>
  <trd lng="es">animar</trd>
  <trd lng="fr">animer</trd>
  <trd lng="hu">inspir&aacute;l</trd>
  <trd lng="sk">o&zcaron;ivi&tcaron;</trd>
</drv>

<drv mrk="anim.egal0a">
  <kap>egal<tld/>a</kap>
  <snc mrk="anim.egal0a.PSI">
    <dif>
      <ref tip="super" cel="trankv.0a.anime">Trankvila</ref>, ne tu&scirc;ata de
      emocioj:
      <ekz>
        penante aspekti egal<tld/>a, &scirc;i aldiris: &ndash; Vere, vi igas min
        dezirema, renkonti tiun mirinda&jcirc;on
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 23a, paca Betra&hcirc;</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="seren.0a.anime">serena <sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>d'<ind>humeur</ind> &eacute;gale</trd>,
    <trd>serein<klr tip="amb"> (sans &eacute;motions)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="anim.egal0eco">
  <kap>egal<tld/>eco</kap>
  <snc mrk="anim.egal0eco.PSI">
    <dif>
      <ref tip="super" cel="trankv.0o.anima">Trankvilo</ref> pro konstanta
      humoro:
      <ekz>
        tamen li ne perdis sian kutiman egal<tld/>econ
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 12a, Lemuel kaj
        Adaha</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_a;&c_k;&c_o;&c_j;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_y;&c_ja;&c_k;&c_a;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_a;&c_r;&c_u;&c_sh;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_r;&c_a;&c_w;&c_n;&c_a;&c_v;&c_a;&c_zh;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_w;&c_n;&c_a;&c_d;&c_u;&c_sh;&c_sh;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">equ&agrave;nimement</trd>
  <trd lng="de">Ausgeglichenheit</trd>
  <trd lng="es">ecuanimidad</trd>
  <trdgrp lng="fr">
    <trd>&eacute;galit&eacute; de caract&egrave;re</trd>,
    <trd>&eacute;quanimit&eacute;</trd>
  </trdgrp>
  <trd lng="hu">egykedv&udblac;s&eacute;g</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_v;&c_o;&c_z;&c_m;&c_u;&c_t;&c_i;&c_m;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_o;&c_k;&c_o;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>,
    <trd>&c_u;&c_r;&c_a;&c_v;&c_n;&c_o;&c_v;&c_je;&c_sh;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_h;&c_l;&c_a;&c_d;&c_n;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_je;</trd>,
    <trd>&c_r;&c_a;&c_v;&c_n;&c_o;&c_d;&c_u;&c_sh;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">vyrovnanos&tcaron;</trd>
</drv>

<drv mrk="anim.facil0a">
  <kap>facil<tld/>a</kap>
  <snc mrk="anim.facil0a.PSI">
    <dif>
      Senpripensa, senkonsidera, neserioza:
      <ekz>
        la virinoj estas tiel facil<tld/>aj
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_je;&c_g;&c_k;&c_a;&c_d;&c_u;&c_m;&c_n;&c_y;</trd>
  <trd lng="de">leichtfertig</trd>
  <trdgrp lng="fr">
    <trd>insouciant</trd>,
    <trd>l&eacute;ger<klr tip="ind"> (insouciant)</klr></trd>
  </trdgrp>
  <trd lng="he">&qof;&lamed; &dalet;&ayin;&tav;</trd>
  <trd lng="hu">k&ouml;nnyelm&udblac;</trd>
  <trd lng="nl">ondoordacht</trd>
  <trd lng="ru">&c_l;&c_je;&c_g;&c_k;&c_o;&c_m;&c_y;&c_s;&c_l;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="anim.facil0ajxo">
  <kap>facil<tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Malserioza, senpripensa ago:
      <ekz>
        &ccirc;u oni povas tiel konstante faradi facil<tld/>a&jcirc;ojn
        <fnt><bib>Marta</bib></fnt>?
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>b&ecirc;tise<klr tip="amb"> (acte irr&eacute;fl&eacute;chi)</klr></trd>,
    <trd>folie<klr tip="amb"> (acte irr&eacute;fl&eacute;chi)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="anim.facil0eco">
  <kap>facil<tld/>eco</kap>
  <snc mrk="anim.facil0eco.PSI">
    <dif>
      Malserioza humoro a&ubreve; karaktero, emo al agado sen kalkulo de sekvoj:
      <ekz>
        mia <klr>...</klr> koro malsa&gcirc;e pu&scirc;is min en &ccirc;i tiun
        facil<tld/>econ
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 9a, Aventura
        voja&gcirc;o</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">l&eacute;g&egrave;ret&eacute;<klr tip="amb"> (manque de s&eacute;rieux)</klr></trd>
</drv>

<drv mrk="anim.grand0a">
  <kap>grand<tld/>a</kap>
  <snc mrk="anim.grand0a.PSI">
    <dif>
      <ref tip="super" cel="nobl.0a">Nobla</ref>, donema, pardonema, senenvia:
      <ekz>
        &ccirc;u ne konvenas al la venkinto esti grand<tld/>a
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 13a</lok></fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_je;&c_l;&c_ib;&c_k;&c_a;&c_d;&c_u;&c_sh;&c_n;&c_y;</trd>
  <trdgrp lng="de">
    <trd>gro&szlig;herzig</trd>,
    <trd>gro&szlig;z&uuml;gig</trd>
  </trdgrp>
  <trd lng="fr">magnanime</trd>
  <trd lng="nl">groothartig</trd>
  <trd lng="ru">&c_v;&c_je;&c_l;&c_i;&c_k;&c_o;&c_d;&c_u;&c_sh;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="anim.grand0eco">
  <kap>grand<tld/>eco</kap>
  <snc mrk="anim.grand0eco.PSI">
    <dif>
      Kapablo agi noble, pardoneme sen postkolero a&ubreve; malfido:
      <ekz>
        Asirianoj, malgra&ubreve; la barbareco, ne estas malbonaj homoj, se ili
        &scirc;atas la grand<tld/>econ
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 11a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>largeur d'esprit</trd>,
    <trd>magnanimit&eacute;</trd>
  </trdgrp>
</drv>

<drv mrk="anim.kot0ulo">
  <kap>kot<tld/>ulo</kap>
  <snc>
    <dif>
      <ref tip="super" cel="nobl.mal0ulo">Malnoblulo</ref> kun trivialaj
      pensoj, nekapabla de bonaj agoj:
      <ekz>
        fi al la abomeninda kot<tld/>ulo, kiu &scirc;in trompe forlasis
        <fnt><bib>Fab4</bib>, <lok>Sterkskarabo</lok></fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">ignoble individu</trd>
</drv>

<drv mrk="anim.largx0a">
  <kap>lar&gcirc;<tld/>a</kap>
  <snc mrk="anim.largx0a.PSI">
    <dif>
      Volonte toleranta diversajn opiniojn, akceptanta diversajn
      vidpunktojn:
      <ekz>
        li estas lar&gcirc;<tld/>a liberpensulo
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 3a, Lemuel da&ubreve;rigas sian
        esploron</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="toler.0ema">tolerema</ref>
    </refgrp>
  </snc>
  <trd lng="fr">large d'esprit</trd>
</drv>

<drv mrk="anim.largx0eco">
  <kap>lar&gcirc;<tld/>eco</kap>
  <snc mrk="anim.largx0eco.PSI">
    <dif>
      Akceptanta &ccirc;iajn doktrinojn a&ubreve; sintenojn &ccirc;e aliaj homoj, pli volonte
      vidanta iliajn akordojn ol iliajn malakordojn.
    </dif>
  </snc>
  <trd lng="fr">largeur d'esprit</trd>
</drv>

<drv mrk="anim.unu0e">
  <kap>unu<tld/>e</kap>
  <snc>
    <dif>
      &Ccirc;iukonsente.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_n;&c_a;&c_d;&c_u;&c_sh;&c_n;&c_a;</trd>
  <trd lng="ca">un&agrave;nimement</trd>
  <trd lng="de">einstimmig</trd>
  <trd lng="es">unanimemente</trd>
  <trdgrp lng="fr">
    <trd>unanimement</trd>,
    <trd>&agrave; l'<ind>unanimit&eacute;</ind></trd>
  </trdgrp>
  <trd lng="hu">egyhang&uacute;lag</trd>
  <trd lng="nl">eensgezind</trd>
  <trd lng="ru">&c_je;&c_d;&c_i;&c_n;&c_o;&c_d;&c_u;&c_sh;&c_n;&c_o;</trd>
  <trd lng="sk">jednomyse&lcaron;ne</trd>
</drv>
</art>
