<art mrk="$Id: flangx.xml,v 1.18 2025/08/26 10:49:47 revo Exp $">
<kap>
<rad>flan&gcirc;</rad>/o
<fnt><bib>PVEF</bib></fnt></kap>
<drv mrk="flangx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Elstaranta randa&jcirc;o por fortikigi pecon a&ubreve; por ebligi &gcirc;ian
      kunigon kun alia:
      <ekz>tuba <tld/>o;</ekz>
      <ekz>
        <tld/>o fiksita, movebla, surlutita, surnitita, sur&scirc;ra&ubreve;bita;
      </ekz>
      <ekz>
        <tld/>a bolto, <tld/>o&scirc;ra&ubreve;bingo;
      </ekz>
      <ekz>
        por kunigo oni uzas <tld/>ojn fiksitajn &ccirc;e la tubekstremoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=171198"
              >Junto (hidra&ubreve;liko)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="junt.0o.ilo">junto<sncref/></ref>,
      <ref cel="muf.0o.kunigilo">mufo<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_f;&c_l;&c_ja;&c_n;&c_je;&c_c;</trd>
  <trd lng="de">Flansch</trd>
  <trd lng="es">brida</trd>
  <trdgrp lng="fr">
    <trd>collerette<klr tip="ind"> (techn.)</klr></trd>,
    <trd>bride<klr tip="ind"> (collerette)</klr></trd></trdgrp>
  <trdgrp lng="hu">
    <trd>perem</trd>,
    <trd>karima</trd>
  </trdgrp>
  <trd lng="ja">&#x30D5;&#x30E9;&#x30F3;&#x30B8;</trd>
  <trd lng="nl">flens</trd>
  <trd lng="pl">ko&lstroke;nierz</trd>
  <trd lng="pt">flange</trd>
  <trd lng="ru">&c_f;&c_l;&c_a;&c_n;&c_je;&c_c;</trd>
  <trdgrp lng="uk">
    <trd>&c_f;&c_l;&c_a;&c_n;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_je;&c_b;&c_o;&c_r;&c_d;&c_a; (&c_k;&c_o;&c_l;&c_je;&c_s;&c_a;)</trd>,
    <trd>&c_g;&c_r;&c_je;&c_b;&c_ib;&c_n;&c_mol;</trd>,
    <trd>&c_v;&c_i;&c_s;&c_t;&c_u;&c_p;</trd>,
    <trd>&c_b;&c_o;&c_r;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6CD5;&#x5170;&#x76D8; <pr>f&#x1CE;l&aacute;np&aacute;n</pr></trd>,
    <trd>&#x6CD5;&#x5170; <pr>f&#x1CE;l&aacute;n</pr></trd>
  </trdgrp>
</drv>
</art>
