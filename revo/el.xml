      <trdgrp lng="pl">
        <trd>z</trd>,
        <trd>ze</trd>
      </trdgrp>
        <trdgrp lng="th">
        <trd>&#3592;&#3634;&#3585;</trd>,
        <trd>&#3651;&#3609;</trd>
      </trdgrp>
    </snc>
    <snc mrk="el.0.materialo">
      <dif>
        La materialon, kiu konsistigas ion; la erojn de tuto:
        <ekz>
          ili ne &ccirc;iam estas faritaj <tld/> tolo
          <fnt><bib>F</bib>, <lok>&FE; 35</lok></fnt>;
        </ekz>
        <ekz>
          kolzono <tld/> perloj;
        </ekz>
        <ekz>
          statuoj <tld/> marmoro
          <fnt><aut>&AtZ;</aut>, <vrk>Fabeloj,
          Malgranda Tuk</vrk></fnt>;
        </ekz>
        <ekz>
          mantelo <tld/> nigra veluro
          <fnt><aut>&AtZ;</aut>, <vrk>Fabeloj,
          Dio de dormo</vrk></fnt>;
        </ekz>
        <ekz>
          man&gcirc;o <tld/> verda&jcirc;o
          <fnt><bib>MT</bib>, <lok>&Sen; 15:17</lok></fnt>;
        </ekz>
        <ekz>
          pudingo <tld/> pomoj;
        </ekz>
        <ekz>
          domo <tld/> brikoj;
        </ekz>
        <ekz>
          vesto <tld/> flanelo;
        </ekz>
        <ekz>
          tegmento <tld/> verdaj folioj;
        </ekz>
        <ekz>
          plekti kronon <tld/> asperuloj
          <fnt>
            <bib>Fab4</bib>
            <lok>Peco da perlovico</lok>
          </fnt>,     
          <tld/> blankaj lilioj
          <fnt>
            <bib>Fab1</bib>
            <lok>La virineto de maro</lok>
          </fnt>,
          <tld/> dornoj
          <fnt>
            <bib>NT</bib>
            <lok>Johano 19:2</lok>
          </fnt>
          <fnt>
            <bib>NT</bib>
            <lok>Mateo 27:29</lok>
          </fnt>;
        </ekz>
        <ekz>
          vortaro <tld/> &ccirc;irka&ubreve; mil vortoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
        </ekz>
        <ekz>
          haremo <tld/> dek virinoj;
        </ekz>
        <ekz>
          <uzo tip="stl">FIG</uzo>
          nia urbo havas pasinton <tld/> gloro;
        </ekz>
        <ekz>
          <uzo tip="stl">FIG</uzo>
          mia &gcirc;ojo estis kovrata de malhela vualo <tld/>
          malgajeco;
        </ekz>
        <ekz>
          la tuta trezoro <tld/> scienco amasigita de la homaro;
        </ekz>
        <ekz>
          hereda&jcirc;o <tld/> tradicioj;
        </ekz>
        <ekz>
          miksa&jcirc;o <tld/> timo kaj respekto;
        </ekz>
        <ekz>
          aro <tld/> eminentaj personoj
          (<ref tip="vid" cel="da.0">da</ref>);
        </ekz>
        <ekz>
          la verko konsistas <tld/> kvin partoj.
        </ekz>
      </dif>
      <trdgrp lng="pl">
        <trd>z</trd>,
        <trd>ze</trd>
      </trdgrp>
        <trd lng="th">&#3592;&#3634;&#3585;</trd>
    </snc>
