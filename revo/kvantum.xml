<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kvantum.xml,v 1.19 2025/07/20 06:00:24 revo Exp $">
<kap>
  <rad>kvantum</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="kvantum.0o">
  <kap><tld/>o</kap>
  <snc mrk="kvantum.0o.unuo">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Elementa <ref tip="super" cel="unu.0o.abstrakta">unuo</ref> de fizika grando:
      <ekz>
        <tld/>o de energio, de movkvanta
        momanto, de elektro;
      </ekz>
      <ekz>
        Einstein ja ricevis la Nobel-premion pri fiziko en 1921 <klr>[por]</klr> la
        klarigo de la fotoelektra efiko, <klr>[kiu]</klr> spronis la <tld/>an teorion
        <fnt>
          <bib>Kon11</bib>
          <lok>La homo, kiu komprenis la universan lingvon</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>fiziko bonege traktas la malgrandan mondon de atomoj kaj nukleoj
        <fnt>
          <bib>Monato</bib>
          <aut>Bruce Sherwood</aut>
          <vrk><url ref="&Monato;/2010/010002.php"
              >Naturo de scienco</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="pl">kwant</trd>
  <trd lng="pt">quantum</trd>
  </snc>
  <snc mrk="kvantum.0o.korpusklo">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Elementa korpusklo kun nula a&ubreve; entjera spino;
      <ref tip="sin" cel="boson.0o">bosono</ref>:
      <ekz>
        en Italio <klr>[&#8230;]</klr> estis konstruita la unua <tld/>a laboratorio de la
        mondo, aperis la unuaj modeloj de fotonaj proceziloj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011214.php"
              >2013: dek donacoj al la posteuloj</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="foton.0o">fotono</ref>
    <refgrp tip="vid">
      <ref cel="atom.0o">atomo</ref>,
      <ref cel="elektr14.0o">elektrono</ref>
    </refgrp>
  <trdgrp lng="pt">
     <trd>quantum</trd>,
     <trd>b&oacute;son</trd>
   </trdgrp>
  </snc>
  <trd lng="be">&c_k;&c_v;&c_a;&c_n;&c_t;</trd>
  <trd lng="ca">qu&agrave;ntum</trd>
  <trd lng="cs">kvantum</trd>
  <trd lng="cv">&c_k;&c_v;&c_a;&c_n;&c_t;</trd>
  <trd lng="de">Quant</trd>
  <trd lng="en">quantum</trd>
  <trd lng="fa">&f_k;&a_w;&a_A;&a_n;&a_t;&a_w;&a_m;</trd>
  <trd lng="fr">quantum</trd>
  <trd lng="hu">kvantum</trd>
  <trd lng="ja">&#37327;&#23376;  <pr>&#12426;&#12423;&#12358;&#12375;</pr></trd>
  <trd lng="nl">kwantum</trd>
  <trd lng="ru">&c_k;&c_v;&c_a;&c_n;&c_t;</trd>
  <trd lng="sk">kvantum</trd>
  <trd lng="uk">&c_k;&c_v;&c_a;&c_n;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#37327;&#23376; <pr>li&agrave;ngz&#464;</pr></trd>,
    <trd>&#20221;&#37327; <pr>f&egrave;nli&agrave;ng</pr></trd>,
    <trd>&#20998;&#37197;&#37327; <pr>f&emacron;np&egrave;ili&agrave;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kvantum.0mehxaniko">
  <kap><tld/>me&hcirc;aniko</kap>
  <snc mrk="kvantum.0mehxaniko.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Me&hcirc;aniko, en kiu fizikaj grandoj estas reprezentataj per
      matricoj a&ubreve; operatoroj.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_v;&c_a;&c_n;&c_t;&c_a;&c_v;&c_a;&c_ja;
  &c_m;&c_e;&c_h;&c_a;&c_n;&c_ib;&c_k;&c_a;</trd>
  <trd lng="ca">mec&agrave;nica qu&agrave;ntica</trd>
  <trdgrp lng="cv">
    <trd>&c_k;&c_v;&c_a;&c_n;&c_t; &c_m;&c_je;&c_h;&c_a;&c_n;&c_i;&c_k;&c_i;</trd>,
    <trd>&c_k;&c_v;&c_a;&c_n;&c_t;&c_l;&c_a; &c_m;&c_je;&c_h;&c_a;&c_n;&c_i;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Quantenmechanik</trd>
  <trd lng="en">quantum mechanics</trd>
  <trd lng="fa">&a_m;&f_k;&a_A;&a_n;&f_y;&f_k; &f_k;&a_w;&a_A;&a_n;&a_t;&a_w;&a_m;&f_y;</trd>
  <trd lng="fr">m&eacute;canique quantique</trd>
  <trd lng="hu">kvantummechanika</trd>
  <trd lng="nl">kwantummechanica</trd>
  <trd lng="pl">mechanika kwantowa</trd>
  <trd lng="pt">mec&acirc;nica qu&acirc;ntica</trd>
  <trd lng="ru">&c_k;&c_v;&c_a;&c_n;&c_t;&c_o;&c_v;&c_a;&c_ja;
  &c_m;&c_je;&c_h;&c_a;&c_n;&c_i;&c_k;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#37327;&#23376;&#21147;&#23398; <pr>li&agrave;ngz&#464;l&igrave;xu&eacute;</pr></trd>,
    <trd>&#37327;&#23376;&#21147;&#23416; <pr>li&agrave;ngz&#464;l&igrave;xu&eacute;</pr></trd>
  </trdgrp>
</drv>
</art>
