<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: irigac.xml,v 1.15 2021/04/19 07:02:24 revo Exp $">
<kap>
  <rad>irigac</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="irigac.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="irigac.0o.AGR">
  <uzo tip="fak">AGR</uzo>
    <dif>
      <ref tip="dif" cel="irigaci.0o.AGR">Irigacio</ref>:
      <ekz>
        li okupi&gcirc;is &ccirc;efe pri legomoj kaj <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=429889"
              >L&aacute;szl&oacute; Csel&odblac;tei</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_r;&c_y;&c_g;&c_a;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_a;&c_b;&c_v;&c_a;&c_d;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_a;&c_r;&c_a;&c_sh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="fr">irrigation</trd>
  <trdgrp lng="pl">
    <trd>irygacja</trd>,
    <trd>nawodnienie</trd>
  </trdgrp>
  <trd lng="pt">irriga&ccedil;&atilde;o</trd>
</drv>

<drv mrk="irigac.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="irigac.0i.AGR">
    <dif>
      <ref tip="dif" cel="irigaci.0i.AGR">Irigacii</ref>:
      <ekz>
        el &gcirc;i la kafonoj <klr>[&#8230;]</klr> &ccirc;iam &ccirc;erpis akvon por <tld/>i la malmultajn
        kulturejojn, kiujn ili posedas en la ebena&jcirc;o
        <fnt>
          <aut>Ignazio Silone, trad. A. Angelo k. J. van Scheepen</aut>
          <vrk><url ref="http://esperanto.davidgsimpson.com/inko/196-8.pdf"
              >Fontamara</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_v;&c_a;&c_d;&c_n;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_r;&c_a;&c_sh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="en">to make irriguous</trd>
  <trd lng="fr">irriguer</trd>
  <trdgrp lng="pl">
    <trd>irygowa&cacute;</trd>,
    <trd>nawadnia&cacute;</trd>
  </trdgrp>
</drv>
</art>
