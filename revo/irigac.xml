<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>irigac(i)</rad>/o
</kap>
<drv mrk="irigac.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Alkonduko de akvo per tuboj, kanaloj ktp, sur la supra&jcirc;on de
      tro seka grundo. 
    </dif>
  </snc>
  <trd lng="de">Bew&auml;sserung</trd>
  <trdgrp lng="nl">
    <trd>irrigatie</trd>,
    <trd>bevloeiing</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_r;&c_r;&c_i;&c_g;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_o;&c_b;&c_v;&c_o;&c_d;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_o;&c_r;&c_o;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
<drv mrk="irigac.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Alkonduki akvon sur kulturatan terspacon.  
    </dif>
  </snc>
  <trd lng="de">bew&auml;ssern</trd>
  <trdgrp lng="nl">
    <trd>bevloeien</trd>,
    <trd>irrigeren</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_v;&c_o;&c_d;&c_n;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_r;&c_o;&c_sh;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
