<art mrk="$Id$">
<kap>
  <rad>irigac</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="irigac.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="irigac.0o.AGR">
  <uzo tip="fak">AGR</uzo>
    <ref tip="dif" cel="irigaci.0o.AGR">irigacio</ref>
  </snc>
  <trd lng="pt">irriga&#231;&#227;o</trd>
</drv>

<drv mrk="irigac.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="irigac.0i.AGR">
    <ref tip="dif" cel="irigaci.0i.AGR">irigacii</ref>
  </snc>
</drv>
</art>
