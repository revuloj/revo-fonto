<kap>
  <rad>&jcirc;am</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="jxam.0i">
  <kap><tld/>i</kap>
  <snc mrk="jxam.0i.TEL">
    <uzo tip="fak">TEL</uzo>
    <dif>
      <ref tip="super" cel="help.mal0i.ion">Malhelpi</ref> la distingon de informo super&scirc;utante &gcirc;in
      per bruo:
      <ekz>
        Radio Libera E&ubreve;ropo <klr>[&#8230;tiam]</klr> ne estis <tld/>ita kaj &gcirc;i estis la plej grava informa fonto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=514244"
              >Dua Kabineto Imre Nagy</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la parol<tld/>ilon eblas direkti al unuopulo
        kiel pafilon
        <fnt><aut>Arono kaj Karlina</aut>, <vrk>Radio Verda</vrk>,
        <lok><url ref="http://radioverda.com/programoj/2012/3/11/rv189-20120311.html">
        RV189</url>, 2012-03-11</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="entrop.0o.FIZ">entropio<sncref/></ref>,
      <ref cel="nebul.0igi">nebuligi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_l;&c_u;&c_sh;&c_y;&c_c;&c_mol; <klr>(&c_n;&c_a;&c_p;&c_r;. &c_r;&c_a;&c_d;&c_y;&c_jo;&c_p;&c_je;&c_r;&c_a;&c_d;&c_a;&c_ch;&c_u;)</klr></trd>,
    <trd>&c_p;&c_a;&c_d;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol; <klr>(&c_r;&c_a;&c_d;&c_y;&c_jo;&c_s;&c_y;&c_g;&c_n;&c_a;&c_l;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>st&ouml;ren <klr>(&Uuml;bertragung)</klr></trd>,
    <trd>verrauschen <klr>(Signal)</klr></trd>
  </trdgrp>
  <trd lng="fa">&f_p;&a_A;&a_r;&a_A;&a_z;&f_y;&a_t; &a_A;&a_n;&a_d;&a_A;&a_kh;&a_t;&a_n;</trd>
  <trd lng="fr">brouiller<klr> (t&eacute;l&eacute;com.)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_l;&c_u;&c_sh;&c_i;&c_t;&c_i; (&c_p;&c_je;&c_r;&c_je;&c_d;&c_a;&c_ch;&c_u;)</trd>,
    <trd>&c_s;&c_t;&c_v;&c_o;&c_r;&c_ju;&c_v;&c_a;&c_t;&c_i; &c_p;&c_je;&c_r;&c_je;&c_sh;&c_k;&c_o;&c_d;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#28151;&#28102; <pr>h&ugrave;nxi&aacute;o</pr></trd>
</drv>
