  </trdgrp>
  <trd lng="fa">&f_p;&a_A;&a_r;&a_A;&a_z;&f_y;&a_t; &a_A;&a_n;&a_d;&a_A;&a_kh;&a_t;&a_n;</trd>
  <trd lng="fr">brouiller<klr> (t&eacute;l&eacute;com.)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_g;&#x43B;&c_u;&c_sh;&c_i;&c_t;&c_i; (&#x43F;&c_je;&c_r;&c_je;&c_d;&c_a;&c_ch;&c_u;)</trd>,
    <trd>&c_s;&c_t;&c_v;&#x43E;&c_r;&#x44E;&c_v;&c_a;&c_t;&c_i; &#x43F;&c_je;&c_r;&c_je;&c_sh;&#x43A;&#x43E;&c_d;&c_i;</trd>
  </trdgrp>
</drv>
