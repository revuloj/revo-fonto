  <trdgrp lng="be">
    <trd>&c_g;&c_l;&c_u;&c_sh;&c_y;&c_c;&c_mol; <klr>(&c_n;&c_a;&c_p;&c_r;. &c_r;&c_a;&c_d;&c_y;&c_jo;&c_p;&c_je;&c_r;&c_a;&c_d;&c_a;&c_ch;&c_u;)</klr></trd>,
