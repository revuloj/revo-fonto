<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: petros.xml,v 1.28 2023/09/03 12:45:07 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>petrosel</rad>/o
</kap>

<drv mrk="petros.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">Petroselinum</trd>),
      uzata kiel <ref tip="lst" cel="spic.0o.KUI" lst="voko:spicoj">spic</ref>herbo:
      <ekz>
        jen <tld/>o kaj floranta timiano
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de unu patrino</lok>
        </fnt>;
      </ekz>
      <ekz>
        telero da kukurbetoj je <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Sen Rodin</aut>
          <vrk><url ref="&Monato;/2006/008905.php">Negro i&gcirc;is nigrulo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj fre&scirc;aj legomoj kaj herboj (ekzemple brokolo, <tld/>o, kurkumo)
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van De Velde</aut>
          <vrk><url ref="&Monato;/2013/010934.php">Sablohorlo&gcirc;o for&scirc;ovas piramidon</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        homoj, ligitajn en faskon kvaza&ubreve; <tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/a/a6/Petroselinum_crispum_kz2.jpg/440px-Petroselinum_crispum_kz2.jpg" prm="CC BY-SA 4.0">krispa <tld/>o (Petroselinum crispum) en la botanika &gcirc;ardeno de Brooklyn
        <fnt>
          <aut>Krzysztof Ziarnek, Kenraiz, propra verko</aut>
          <lok><url ref="&WCW;/File:Petroselinum_crispum_kz2.jpg">&WCW;/File:Petroselinum_crispum_kz2.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_p;&c_ja;&c_t;&c_r;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trd lng="bg">&c_m;&c_a;&c_g;&c_d;&c_a;&c_n;&c_o;&c_z;</trd>
  <trd lng="bo">&#xF56;&#xF66;&#xF7C;&#xF51;&#xF0B;&#xF53;&#xF58;&#xF66;&#xF0B;&#xF51;&#xF54;&#xF63;&#xF0B;&#xF60;&#xF5B;&#xF7C;&#xF58;&#xF66;&#xF0B;</trd>
  <trd lng="br">perisilh</trd>
  <trd lng="cs">petr&zcaron;el</trd>
  <trd lng="de">Petersilie</trd>
  <trd lng="en">parsley</trd>
  <trd lng="es">perejil</trd>
  <trd lng="fa">&a_j;&a_ayn;&a_f;&a_r;&f_y;</trd>
  <trd lng="fr">persil</trd>
  <trd lng="hu">petrezselyem</trd>
  <trd lng="id">peterseli</trd>
  <trd lng="ja">&#x30D1;&#x30BB;&#x30EA;</trd>
  <trd lng="nl">peterselie</trd>
  <trd lng="pl">pietruszka</trd>
  <trd lng="ru">&c_p;&c_je;&c_t;&c_r;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trd lng="sk">petr&zcaron;len</trd>
  <trd lng="tr">maydanoz</trd>
  <trd lng="uk">&#x43F;&#x435;&#x442;&#x440;&#x443;&#x448;&#x43A;&#x430;</trd>
</drv>
</art>
