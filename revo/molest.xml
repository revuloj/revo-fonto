<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: molest.xml,v 1.22 2025/11/28 06:46:32 revo Exp $">
<kap>
  <rad>molest</rad>/i <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="molest.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="molest.0i.mistrakti">
    <dif>
      <ref tip="super" cel="trakt.mis0i">Mistrakti</ref> per
      insultoj, batoj ks; agrese <ref tip="super" cel="komfor.mal0igi">malkomfortigi</ref>:
      <ekz>
        &ccirc;u vi pensas, <klr>[&#x2026;]</klr> ke vi povas kaprice <tld/>i? vi
        miskalkulas
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 74a, volumo 2a, p.
        563a</lok></fnt>!
      </ekz>
      <ekz>
        grupo da junuloj <klr>[&#x2026;]</klr> <tld/>as junan judan knabon, &ccirc;ar li estas
        surstrate rekonebla
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="&Monato;/2005/008715.php">Nia estonta E&ubreve;ropo ...</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        infana&gcirc;aj traviva&jcirc;oj <klr>[inkluzivas]</klr> la ekstremajn kazojn de <tld/>ado &ndash;
        seksa a&ubreve; perforta
        <fnt>
          <bib>Monato</bib>
          <aut>Lenio Marobin</aut>
          <vrk><url ref="&Monato;/2013/010927.php">Profitinda infana&gcirc;o</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[roz-vendistoj]</klr> neatendite enfalas en publikajn lokojn, ekzemple
        restoraciojn kaj kafejojn, iufoje iom <tld/>e
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2012/010581.php">Lasta spiro, lasta
          espero</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="agres.0i">agresi</ref>,
      <ref cel="cxikan.0i.gxeni">&ccirc;ikani<sncref/></ref>,
      <ref cel="gxen.0i">&gcirc;eni</ref>,
      <ref cel="kompro1.0i">kompromiti</ref>,
      <ref cel="uz.mis0i">misuzi</ref>,
      <ref cel="mok.0i">moki</ref>,
      <ref cel="fort.per0i">perforti</ref>,
      <ref cel="ted.0i">tedi</ref>,
      <ref cel="trud.sin0i">sin trudi</ref>,
      <ref cel="turmen.0i">turmenti</ref>
    </refgrp>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>&gcirc;eni</ke>.
  </rim>
  <rim>
    <em><tld lit="M"/>ado</em> okazas kun malbona, malamika intenco. <em>&Gcirc;enado</em> tion ne implicas,
    okazas ofte subjektive kaj plie, fonto de &gcirc;eno povas esti iu ajn afero.
    En Esperanto, <em>&ccirc;ikanado</em> estas pli subtila ol molestado kaj ne inkluzivas korpajn atakojn.
    <aut>Wolfram Diestel</aut>
  </rim>
  <trdgrp lng="be">
    <trd>&c_g;&c_n;&c_je;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_g;&c_n;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_n;&c_je;&c_v;&c_a;&c_zh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dot&iacute;rat</trd>,
    <trd>hrub&ecaron; jednat</trd>,
    <trd>obt&ecaron;&zcaron;ovat</trd>,
    <trd>ur&aacute;&zcaron;liv&ecaron; napadat</trd>,
    <trd>&scaron;patn&ecaron; zach&aacute;zet</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>bel&auml;stigen</trd>,
    <trd>mobben</trd>,
    <trd>schikanieren</trd>,
    <trd><ind>&uuml;bergriffig</ind> werden</trd>,
    <trd>misshandeln</trd>,
    <trd>missbrauchen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>to accost</trd>,
    <trd>molest</trd>
  </trdgrp>
  <trd lng="es">molestar</trd>
  <trdgrp lng="fr">
    <trd>brutaliser</trd>,
    <trd>molester</trd>,
    <trd>rudoyer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>zaklat</trd>,
    <trd>moleszt&aacute;l</trd>,
    <trd>b&aacute;nt</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3044;&#x3058;&#x3081;&#x308B;</trd>,
    <trd>&#x8650;&#x5F85;&#x3059;&#x308B;  <pr>&#x304E;&#x3083;&#x304F;&#x305F;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x82E6;&#x3057;&#x3081;&#x308B;  <pr>&#x304F;&#x308B;&#x3057;&#x3081;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">molesteren</trd>
  <trd lng="pl">molestowa&cacute;</trd>
  <trdgrp lng="pt">
   <trd>molestar</trd>,
   <trd>destratar</trd>,
   <trd>machucar</trd>,
   <trd>ferir</trd>
   <trd>incomodar</trd>
  </trdgrp><trd lng="pt">molestar</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_u;&c_b;&c_o;
&c_o;&c_b;&c_r;&c_a;&c_shch;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_t;&c_je;&c_s;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nepriate&lcaron;sky</trd>,
    <trd>ur&aacute;&zcaron;livo jedna&tcaron;</trd>,
    <trd>zaobch&aacute;dza&tcaron; <klr>(s nie&ccaron;&iacute;m, niek&yacute;m)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_i;&c_s;&c_t;&c_a;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_ch;&c_ib;&c_p;&c_l;&c_ja;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_s;&c_ib;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_s;&c_k;&c_ib;&c_p;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_d;&c_i;&c_r;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_d;&c_o;&c_s;&c_a;&c_zh;&c_d;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_d;&c_o;&c_k;&c_u;&c_ch;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_m;&c_u;&c_ch;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_s;&c_l;&c_ib;&c_d;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_g;&c_n;&c_o;&c_b;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_g;&c_n;&c_o;&c_b;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_u;&c_t;&c_i;&c_s;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_g;&c_r;&c_u;&c_b;&c_o;/&c_r;&c_ib;&c_z;&c_k;&c_o;/&c_s;&c_u;&c_v;&c_o;&c_r;&c_o; &c_p;&c_o;&c_v;&c_o;&c_d;&c_i;&c_t;&c_i;&c_s;&c_ja; &c_z; &c_k;&c_i;&c_m;-&c_n;.</trd>,
    <trd>&c_t;&c_r;&c_je;&c_t;&c_i;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_o;&c_p;&c_i;&c_h;&c_a;&c_t;&c_i; (&c_k;&c_o;&c_g;&c_o;</trd>,
    <trd>&c_k;&c_i;&c_m;)</trd>,
    <trd>&c_p;&c_o;&c_p;&c_i;&c_h;&c_a;&c_t;&c_i;&c_s;&c_ja; (&c_k;&c_i;&c_m;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6253;&#x6270; <pr>d&#x1CE;r&#x1CE;o</pr></trd>,
    <trd>&#x6253;&#x64FE; <pr>d&#x1CE;r&#x1CE;o</pr></trd>,
    <trd>&#x6414; <pr>s&amacron;o</pr></trd>,
    <trd>&#x6B3A;&#x4FAE; <pr>q&imacron;w&#x1D4;</pr></trd>,
    <trd>&#x8D77;&#x96FE; <pr>q&#x1D0;w&ugrave;</pr></trd>,
    <trd>&#x8D77;&#x9727; <pr>q&#x1D0;w&ugrave;</pr></trd>,
    <trd>&#x4F7F;&#x611F;&#x5230;&#x4E0D;&#x4FBF; <pr>sh&#x1D0;g&#x1CE;nd&agrave;ob&ugrave;bi&agrave;n</pr></trd>,
    <trd>&#x4F7F;&#x611F;&#x5230;&#x56F0;&#x96BE; <pr>sh&#x1D0;g&#x1CE;nd&agrave;ok&ugrave;nnan</pr></trd>,
    <trd>&#x4F7F;&#x611F;&#x5230;&#x56F0;&#x96E3; <pr>sh&#x1D0;g&#x1CE;nd&agrave;ok&ugrave;nnan</pr></trd>,
    <trd>&#x60F9; <pr>r&ecaron;</pr></trd>,
    <trd>&#x70E6; <pr>f&aacute;n</pr></trd>,
    <trd>&#x7169; <pr>f&aacute;n</pr></trd>,
    <trd>&#x5B58;&#x5FC3;&#x5201;&#x96BE; <pr>c&uacute;nx&imacron;ndi&amacron;on&aacute;n</pr></trd>,
    <trd>&#x5B58;&#x5FC3;&#x5201;&#x96E3; <pr>c&uacute;nx&imacron;ndi&amacron;on&aacute;n</pr></trd>
  </trdgrp>
</drv>
