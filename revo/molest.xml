<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>molest</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="molest.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="trakt.mis0i">Mistrakti</ref> per batoj,
      supra&jcirc;aj vundadoj a&ubreve; similaj:
      <ekz>
        &ccirc;u vi pensas, <klr>...</klr> ke vi povas kaprice <tld/>i? Vi
        miskalkulas
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 74a, p. 563a</lok></fnt>!
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>brutaliser</trd>,
    <trd>molester</trd>,
    <trd>rudoyer</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_u;&c_b;&c_o; &c_o;&c_b;&c_r;&c_a;&c_shch;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_t;&c_je;&c_s;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>

