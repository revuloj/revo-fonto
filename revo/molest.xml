<drv mrk="molest.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="molest.0i.mistrakti">
    <dif>
      <ref tip="super" cel="trakt.mis0i">Mistrakti</ref> per 
      insultoj, batoj ks; agrese <ref tip="super" cel="komfor.mal0igi">malkomfortigi</ref>:
      <ekz>
        &ccirc;u vi pensas, <klr>[&#x2026;]</klr> ke vi povas kaprice <tld/>i? vi
        miskalkulas
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 74a, volumo 2a, p.
        563a</lok></fnt>!
      </ekz>
      <ekz>
        grupo da junuloj <klr>[&#x2026;]</klr> <tld/>as junan judan knabon, &ccirc;ar li estas
        surstrate rekonebla
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="&Monato;/2005/008715.php">Nia estonta E&ubreve;ropo ...</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        infana&gcirc;aj traviva&jcirc;oj <klr>[inkluzivas]</klr> la ekstremajn kazojn de <tld/>ado &ndash;
        seksa a&ubreve; perforta
        <fnt>
          <bib>Monato</bib>
          <aut>Lenio Marobin</aut>
          <vrk><url ref="&Monato;/2013/010927.php">Profitinda infana&gcirc;o</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[roz-vendistoj]</klr> neatendite enfalas en publikajn lokojn, ekzemple
        restoraciojn kaj kafejojn, iufoje iom <tld/>e
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2012/010581.php">Lasta spiro, lasta
          espero</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
