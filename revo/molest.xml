<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: molest.xml,v 1.13 2020/11/16 14:32:26 revo Exp $">
<kap>
  <rad>molest</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="molest.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="molest.0i.mistrakti">
    <dif>
      <ref tip="super" cel="trakt.mis0i">Mistrakti</ref> per 
      insultoj, batoj ks; agrese <ref tip="super" cel="komfor.mal0igi">malkomfortigi</ref>:
      <ekz>
        &ccirc;u vi pensas, <klr>[&#8230;]</klr> ke vi povas kaprice <tld/>i? vi
        miskalkulas
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 74a, volumo 2a, p.
        563a</lok></fnt>!
      </ekz>
      <ekz>
        grupo da junuloj <klr>[&#8230;]</klr> <tld/>as junan judan knabon, &ccirc;ar li estas
        surstrate rekonebla
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="https://www.monato.be/2005/008715.php">Nia estonta E&ubreve;ropo ...</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        infana&gcirc;aj traviva&jcirc;oj <klr>[inkluzivas]</klr> la ekstremajn kazojn de <tld/>ado &ndash;
        seksa a&ubreve; perforta
        <fnt>
          <bib>Monato</bib>
          <aut>Lenio Marobin</aut>
          <vrk><url ref="https://www.monato.be/2013/010927.php">Profitinda infana&gcirc;o</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[roz-vendistoj]</klr> neatendite enfalas en publikajn lokojn, ekzemple
        restoraciojn kaj kafejojn, iufoje iom <tld/>e
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.be/2012/010581.php">Lasta spiro, lasta espero</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="agres.0i">agresi</ref>,
    <ref cel="cxikan.0i.gxeni">&ccirc;ikani<sncref/></ref>,
    <ref cel="gxen.0i">&gcirc;eni</ref>,
    <ref cel="kompro1.0i">kompromiti</ref>,
    <ref cel="uz.mis0i">misuzi</ref>,
    <ref cel="mok.0i">moki</ref>,
    <ref cel="fort.per0i">perforti</ref>,
    <ref cel="ted.0i">tedi</ref>,
    <ref cel="trud.sin0i">sin trudi</ref>,
    <ref cel="turmen.0i">turmenti</ref>
  </refgrp>
  <trdgrp lng="cs">
    <trd>dot&iacute;rat</trd>,
    <trd>hrub&ecaron; jednat</trd>,
    <trd>obt&ecaron;&zcaron;ovat</trd>,
    <trd>ur&aacute;&zcaron;liv&ecaron; napadat</trd>,
    <trd>&scaron;patn&ecaron; zach&aacute;zet</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>bel&auml;stigen</trd>,
    <trd>mobben</trd>,
    <trd>schikanieren</trd>,
    <trd><ind>&uuml;bergriffig</ind> werden</trd>,
    <trd>misshandeln</trd>,
    <trd>missbrauchen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>to accost</trd>,
    <trd>molest</trd>
  </trdgrp>
  <trd lng="es">molestar</trd>
  <trdgrp lng="fr">
    <trd>brutaliser</trd>,
    <trd>molester</trd>,
    <trd>rudoyer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>zaklat</trd>,
    <trd>moleszt&aacute;l</trd>,
    <trd>b&aacute;nt</trd>
  </trdgrp>
  <trd lng="nl">molesteren</trd>
  <trd lng="pl">molestowa&cacute;</trd>
  <trdgrp lng="pt">
   <trd>molestar</trd>,
   <trd>destratar</trd>,
   <trd>machucar</trd>,
   <trd>ferir</trd>   
   <trd>incomodar</trd>
  </trdgrp><trd lng="pt">molestar</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_u;&c_b;&c_o;
&c_o;&c_b;&c_r;&c_a;&c_shch;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_t;&c_je;&c_s;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nepriate&lcaron;sky</trd>,
    <trd>ur&aacute;&zcaron;livo jedna&tcaron;</trd>,
    <trd>zaobch&aacute;dza&tcaron; <klr>(s nie&ccaron;&iacute;m, niek&yacute;m)</klr></trd>
  </trdgrp>
</drv>
</art>
