</drv>
  <drv mrk="per.0anto">
    <kap><tld/>anto</kap>
    <snc mrk="per.0anto.homo">
      <dif>
        Homo, per kies helpo oni traktas aferojn kun aliaj personoj:
        <ekz>
          mi zorgas pri la bonaj rilatoj kun la <tld/>antoj de la
          magazino
          kaj enkasigas &ccirc;iujn kotizojn
          <fnt>
          <aut>-</aut>,
          <vrk>Gerd Jacques</vrk>,
          <bib>Monato</bib>,
          <lok>2003:1, p. 6a</lok>
          </fnt>;
        </ekz>
        <ekz>
          se temas pri fidinda Interret-<tld/>anto, oni certe esploros
          vian plendon
          <fnt><aut>P. Peeraerts</aut>,
          <vrk><url ref="&Monato;/005984.html">trudata reklamo: &ccirc;u
          haltigebla?</url></vrk>,
          <bib>Monato</bib>,
          <lok>1998:4</lok>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="sub">
        <ref cel="agent.0o">agento</ref>,
        <ref cel="deleg.0ito">delegito</ref>,
        <ref cel="komisa.0o">komisaro</ref>,
        <ref cel="komisi">komisiito</ref>
      </refgrp>
    </snc>
  <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_s;&c_ja;&c_r;&c_e;&c_d;&c_n;&c_ib;&c_k;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_v;&c_a;&c_j;&c_d;&c_a;&c_r;</trd>
    </trdgrp>
  <trdgrp lng="cs">
    <trd>prost&rcaron;edn&iacute;k</trd>,
    <trd>z&aacute;stupce</trd>
  </trdgrp>
  <trd lng="de">Vermittler</trd>
  <trdgrp lng="en">
  <trd>agent</trd>, 
    <trd>intermediary</trd>,
    <trd>mediator</trd>
  </trdgrp>
  <trd lng="es">intermediario</trd>
  <trd lng="fr">interm&eacute;diaire<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="hu">k&ouml;zvet&iacute;t&odblac;</trd>
  <trdgrp lng="nl">
      <trd>bemiddelaar</trd>,
      <trd>provider</trd>
    </trdgrp>
  <trd lng="pl">po&sacute;rednik</trd>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_r;&c_je;&c_d;&c_n;&c_i;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>prostredn&iacute;k</trd>,
    <trd>z&aacute;stupca</trd>
  </trdgrp>
</drv>
