    <trd>mit</trd>
  </trdgrp>
  <trd lng="en">by means of</trd>
  <trdgrp lng="es">
    <trd>mediante</trd>,
    <trd>por medio de</trd>,
    <trd>a trav&eacute;s de</trd>
  </trdgrp>
  <trdgrp lng="fr">
