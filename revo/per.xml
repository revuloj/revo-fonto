    <trd>k&ouml;zvet&iacute;t&eacute;s&eacute;vel</trd>
  </trdgrp>
  <trd lng="ja">&#xFF5E;<ind>&#x3092;&#x4ECB;&#x3057;&#x3066;  </ind><pr>&#x3092;&#x304B;&#x3044;&#x3057;&#x3066;</pr></trd>
