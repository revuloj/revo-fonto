<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc><rad>pantalon</rad>/o
</kap>
<drv mrk="pantal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="vest.sur0o">Survesto</ref> kovranta la korpon
      de zono &gcirc;is piedoj, dupartigita tiele, ke &gcirc;i &ccirc;irka&ubreve;as aparte &ccirc;iun
      gambon.
    </dif>
  </snc>
  <trd lng="cs">kalhoty</trd>
  <trd lng="de">Hose</trd>
  <trdgrp lng="en">
    <trd>pants</trd>,
    <trd>trousers</trd>
  </trdgrp>
  <trd lng="fr">pantalon</trd>
  <trdgrp lng="hu">
    <trd>nadr&aacute;g</trd>,
    <trd>pantall&oacute;</trd>
  </trdgrp>
  <trd lng="nl">lange <ind>broek</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_t;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_r;&c_ju;&c_k;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="pantal.0eto">
  <kap><tld/>eto</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="vest.sur0o">Survesto</ref> kovranta la korpon
      de zono &gcirc;is genuoj, &ccirc;irka&ubreve;ante aparte &ccirc;iun
      femuron.
    </dif>
  </snc>
  <trd lng="en">pantaloon</trd>
  <trdgrp lng="hu">
    <trd>r&ouml;vidnadr&aacute;g</trd>,
    <trd>sort</trd>,
    <trd>bermuda</trd>
  </trdgrp>
  <trd lng="nl">korte <ind>broek</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_o;&c_r;&c_t;&c_y;</trd>,
    <trd>&c_t;&c_r;&c_u;&c_s;&c_y;</trd>
  </trdgrp>
</drv>
</art>
