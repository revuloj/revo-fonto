<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc><rad>pantalon</rad>/o</kap>
<drv mrk="pantal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Vesto kovranta la korpon de zono &gcirc;is piedoj, dupartigita tiele, ke
      &gcirc;i &ccirc;irka&ubreve;as aparte &ccirc;iun kruron.
    </dif>
  </snc>
  <trd lng="cs">kalhoty</trd>
  <trd lng="de">Hose</trd>
  <trdgrp lng="en">
    <trd>pants</trd>,
    <trd>trousers</trd>
  </trdgrp>
  <trd lng="fr">pantalon</trd>
  <trd lng="nl">lange <ind>broek</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_t;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_r;&c_ju;&c_k;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="pantal.0eto">
  <kap><tld/>eto</kap>
  <snc>
    <dif>
      Simila vesto malsupreniranta nur &gcirc;is genuoj.
    </dif>
    <ref tip="vid" cel="kalson.0o">kalsono</ref>
    <trd lng="en">pantaloon</trd>
    <trd lng="nl">korte <ind>broek</ind></trd>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_o;&c_r;&c_t;&c_y;</trd>,
    <trd>&c_t;&c_r;&c_u;&c_s;&c_y;</trd>
  </trdgrp>
</drv>
</art>
