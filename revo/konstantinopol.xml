<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Konstantinopol</rad>/o
    </kap>
    <drv mrk="konstantinopol.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">HIS</uzo>
      <snc>
	<dif>
	  Antikva nomo de
	  <ref tip="sin" cel="instanbul.0o">Istanbulo</ref>,
	  post la jaro 324a, kiam Konstanteno la Granda venkoprenis
	  <ref tip="sin" cel="bizanc.0o">Bizancon</ref> kaj alinomis
          &gcirc;in memore al sia venko.
	</dif>
      </snc>
      <trd lng="en">Constantinople</trd>
      <trd lng="el">&Kappa;&omega;&ny;&sigma;&tau;&alfa;&ny;&tau;&jota;&ny;&omikron;&ypsilon_ton;&pi;&omikron;&lambda;&eta;</trd>
      <trd lng="ru">&c_K;&c_o;&c_n;&c_s;&c_t;&c_a;&c_n;&c_t;&c_i;&c_n;&c_o;&c_p;&c_o;&c_l;&c_mol;</trd>
    </drv>

  </art>

<!--
$Log$
-->
</vortaro>