<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>skafald</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="skafald.0o">
  <kap><tld/>o</kap>
  <snc mrk="skafald.0o.KON">
    <uzo tip="fak">KON</uzo>
    <dif>
      <ref tip="super" cel="trab.0aro.KON">Trabaro</ref> per kiu oni 
      pli facile aliras konstruatan a&ubreve; riparatan muron:
      <ekz>
        masonisto staris sur <tld/>o, anta&ubreve; domo, por masoni la fasadon
        
        <fnt>
          <aut>A. Lienhardt</aut>,
          <vrk><url
          ref="http://miresperanto.narod.ru/por_infanoj/shercetoj_por_infanoj/01.htm">
          Amuza legolibro en Esperanto</url></vrk>,
          <lok>1984</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ili bezonas ian <tld/>on, kiu kuntenas la lingvon
        <fnt>
          <aut>M. Cramer</aut>,
          <vrk>El-interna kaj el-ekstera vortprovizo...</vrk>,
          <lok><url ref="http://lingvakritiko.com/">lingva 
          kritiko</url>, 2008-03-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>scaffold</trd>,
    <trd>scaffolding</trd>
  </trdgrp>
  <trd lng="es">andamio</trd>
  <trd lng="fr">&eacute;chafaudage</trd>
  <trd lng="pt">andaime</trd>
</drv>
</art>

