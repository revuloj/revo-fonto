<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: klucxev.xml,v 1.6 2022/08/19 15:37:45 revo Exp $">
<kap><rad>Klu&ccirc;evskaj</rad>/o <fnt><bib>Po&scirc;Atlas</bib></fnt></kap>

<drv mrk="klucxev.K0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref cel="vulkan.0o">Vulkano</ref> en
      <ref tip="malprt" cel="kamcxatk.K0o">Kam&ccirc;atko</ref>, la plej
      alta (4750&#8239;m) kaj la plej aktiva en
      <ref cel="euxraz.E0o">E&ubreve;razio</ref>:
      <ekz>
        la <tld/>o a&gcirc;as 8000 jarojn kaj estas parto de la Pacifika Fajroringo,
        &gcirc;i estas la plej alta inter dekduo de proksimaj tavolvulkanoj, la t.n. <tld/>a grupo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_K;&c_l;&c_ju;&c_ch;&c_a;&c_w;&c_s;&c_k;&c_a;&c_ja; &c_s;&c_o;&c_p;&c_k;&c_a;</trd>,
    <trd>&c_K;&c_l;&c_ju;&c_ch;&c_a;&c_w;&c_s;&c_k;&c_ib; &c_v;&c_u;&c_l;&c_mol;&c_k;&c_a;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kljutschewskoi-Vulkan</trd>,
    <trd>Kljutschewskaja Sopka</trd>
  </trdgrp>
  <trd lng="pl">Kluczewska Sopka</trd>     
  <trd lng="ro">Kliucevskaia Sopka</trd>
  <trd lng="ru">&c_K;&c_l;&c_ju;&c_ch;&c_je;&c_v;&c_s;&c_k;&c_a;&c_ja; &c_s;&c_o;&c_p;&c_k;&c_a;</trd>
</drv>

</art>

