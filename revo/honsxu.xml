<vortaro>
  <art mrk="$Id: honsxu.xml,v 1.8 2024/09/24 07:09:18 afido Exp $">
    <kap><ofc>10</ofc>
      <rad>Hon&scirc;u</rad>/o
    </kap>

<drv mrk="honsxu.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="honsxu.0o.insulo">
    <dif>
      La plej granda el la japanaj
      <ref tip="lst" lst="voko:insuloj" cel="insul.0o.GEOG">insulo</ref>j:
      <ekz>
        en la norda Japanio &ndash; la norda parto de Hon&scirc;uo kaj
        Hokajdo <klr>[&#x2026;]</klr> la
        lo&gcirc;antoj kutimas al malvarma kaj ne&gcirc;a klimato
        <fnt>
          <aut>Kawai Yuka</aut>
          <vrk><url ref="&Monato;/2012/010688.php">Vintra varmo homa</url></vrk>
               <bib>Monato</bib>
          <lok>2012/07, p. 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        Totoro <klr>[&#x2026;]</klr> situas sur la insulo <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=400619">Totoro (urbo)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Honsh&umacron;</trd>
    <trd lng="en">Honshu</trd>
    <trd lng="fr">Honsh&umacron;</trd>
  <trd lng="ja">&#x672C;&#x5DDE; <pr>&#x307B;&#x3093;&#x3057;&#x3085;&#x3046;</pr></trd>
  <trdgrp lng="nl">
    <trd>Honsh&umacron;</trd>,
    <trd>Honsjoe</trd>
  </trdgrp>
    <trd lng="zh">&#x672C;&#x5DDE; <pr>B&#x11B;nzh&#x14D;u</pr></trd>
</snc>
<trdgrp lng="be">
  <trd>&c_H;&c_o;&c_n;&c_s;&c_ju;</trd>,
  <trd>&c_H;&c_a;&c_n;&c_s;&c_ju;</trd>
</trdgrp>
</drv>

