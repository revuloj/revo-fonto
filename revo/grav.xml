  <trdgrp lng="pl"> 
    <trd>waga</trd>, 
    <trd>istota</trd>, 
    <trd>znaczenia</trd>, 
    <trd>donios&lstroke;o&sacute;&cacute;</trd> 
  </trdgrp>
  <trdgrp lng="pt"> 
    <trd>gravidade<klr tip="ind"> (adj.)</klr></trd>, 
    <trd>import&acirc;ncia<klr tip="ind"> (adj.)</klr></trd> 
  </trdgrp>
  <trdgrp lng="ru"> 
    <trd>&c_v;&c_a;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>, 
    <trd>&c_z;&c_n;&c_a;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd> 
  </trdgrp>
  <trdgrp lng="sk">
    <trd>d&ocirc;le&zcaron;itos&tcaron;</trd>,
    <trd>z&aacute;va&zcaron;nos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="sv"> 
    <trd>viktighet</trd>, 
    <trd>betydelse</trd> 
  </trdgrp>
  <trd lng="tp">suli</trd>
</drv> 

