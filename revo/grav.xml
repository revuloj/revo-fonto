<drv mrk="grav.0a"> 
  <kap><ofc>*</ofc><tld/>a</kap> 
  <snc mrk="grav.0a.KOMUNE"> 
    <dif> 
      Atentinda pro sia granda influo a&ubreve; siaj grandaj sekvoj: 
      <ekz>
        ili foriris de li, lasinte lin en <tld/>a malsano
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 24:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        la plej <tld/>aj pleja&gcirc;uloj
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 29:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> staris &ccirc;irka&ubreve;e, akuzante lin pri <klr>[&#x2026;]</klr> <tld/>aj kulpoj
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 25:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        la plej <tld/>aj krimuloj
        <fnt>
          <bib>Fab2</bib>
          <lok>Bildo el kastelremparo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la homo estis tre ri&ccirc;e vestita, li estis videble <tld/>a persono
        <fnt>
          <bib>Fab2</bib>
          <lok>La ombro</lok>
        </fnt>;
      </ekz>
      <ekz>
        li dancas belege, <klr>[&#x2026;]</klr> sed egale <tld/>a demando estas, &ccirc;u li estas
        sufi&ccirc;e instruita
        <fnt>
          <bib>Fab2</bib>
          <lok>La ombro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a festotago
        <fnt>
          <bib>Fab1</bib>
          <lok>Lekanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        estis konfirmado <klr>[&#x2026;]</klr>, tio estis ja <tld/>a tago por ili, el infanoj ili subite
        fari&gcirc;is plenkreskaj
        homoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Sonorilo</lok>
        </fnt>;
      </ekz>
      <ekz> 
        a&ubreve;skultu, &ccirc;ar mi parolos <ind><tld/>a&jcirc;o</ind>n 
        <fnt><bib>MT</bib>, <lok>&Sen; 8:6</lok></fnt>; 
        <trd lng="fr">chose <ind>importante</ind></trd> 
        <trd lng="hu">fontos dolog</trd> 
        <trd lng="nl">iets <ind>belangrijks</ind></trd> 
      </ekz> 
      <ekz>
        ofte de ka&ubreve;zo senenhava venas efiko plej <tld/>a
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif> 
    <refgrp tip="vid"> 
      <ref cel="pez.0a.multe0a">peza<sncref/></ref>,
      <ref cel="serioz.0a">serioza</ref>, 
      <ref cel="signif.0a.FIG">signifa</ref> 
    </refgrp> 
  </snc>
  <trdgrp lng="be"> 
    <trd>&c_v;&c_a;&c_zh;&c_n;&c_y;</trd>, 
    <trd>&c_z;&c_n;&c_a;&c_ch;&c_n;&c_y;</trd>, 
    <trd>&c_ib;&c_s;&c_t;&c_o;&c_t;&c_n;&c_y;</trd>, 
    <trd>&c_s;&c_u;&c_r;'&c_jo;&c_z;&c_n;&c_y;</trd>, 
    <trd>&c_v;&c_a;&c_r;&c_t;&c_y; &c_w;&c_v;&c_a;&c_g;&c_ib;</trd> 
  </trdgrp>
  <trdgrp lng="bg"> 
    <trd>&c_v;&c_a;&c_zh;&c_je;&c_n;</trd>, 
    <trd>&c_s;&c_malmol;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;</trd> 
  </trdgrp>
  <trd lng="bo">&#xF42;&#xF63;&#xF0B;&#xF46;&#xF7A;&#xF53;&#xF0B;&#xF54;&#xF7C;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>d&uring;le&zcaron;it&yacute;</trd>,
    <trd>d&uring;le&zcaron;it&yacute;</trd>,
    <trd>hrub&yacute; (v&aacute;&zcaron;n&yacute;)</trd>,
    <trd>pov&aacute;&zcaron;liv&yacute;</trd>,
    <trd>t&ecaron;&zcaron;k&yacute; <klr>(d&uring;le&zcaron;it&yacute;)</klr></trd>,
    <trd>v&aacute;&zcaron;n&yacute;</trd>,
    <trd>z&aacute;va&zcaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de"> 
    <trd>wichtig</trd>, 
    <trd>bedeutend</trd> 
  </trdgrp>
  <trd lng="en">important</trd>
  <trd lng="es">importante</trd>
  <trdgrp lng="fr"> 
    <trd>important</trd>, 
    <trd>grave</trd>, 
    <trd>gros<klr tip="ind"> (important)</klr></trd>, 
    <trd>marquant</trd> 
  </trdgrp>
  <trdgrp lng="hu"> 
    <trd>fontos</trd>, 
    <trd>s&uacute;lyos<klr tip="amb"> (&aacute;tv.)</klr></trd> 
  </trdgrp>
  <trdgrp lng="nl"> 
    <trd>belangrijk</trd>, 
    <trd>gewichtig</trd>, 
    <trd>aanzienlijk</trd> 
  </trdgrp>
  <trdgrp lng="pl"> 
    <trd>wa&zdot;ny</trd>, 
    <trd>istotny</trd>, 
    <trd>znaczny</trd>, 
    <trd>znacz&aogonek;cy</trd>, 
    <trd>donios&lstroke;y</trd>, 
    <trd>serio<klr> (pot.)</klr></trd> 
  </trdgrp>
  <trdgrp lng="pt"> 
    <trd>grave</trd>, 
    <trd>importante</trd> 
  </trdgrp>
  <trdgrp lng="ru"> 
    <trd>&c_v;&c_a;&c_zh;&c_n;&c_y;&c_j;</trd>, 
    <trd>
    &c_z;&c_n;&c_a;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;
    </trd>, 
    <trd>&c_s;&c_je;&c_r;&c_mol;&c_jo;&c_z;&c_n;&c_y;&c_j;</trd>, 
    <trd>
    &c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;
    </trd> 
  </trdgrp>
  <trdgrp lng="sk">
    <trd>d&ocirc;le&zcaron;it&yacute;</trd>,
    <trd>pov&aacute;&zcaron;liv&yacute;</trd>,
    <trd>v&aacute;&zcaron;ny</trd>,
    <trd>z&aacute;va&zcaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="sv">viktig</trd>
  <trd lng="tp">suli</trd>
</drv> 

<drv mrk="grav.0e"> 
  <kap><tld/>e</kap> 
  <snc mrk="grav.0e.KOMUNE"> 
    <dif> 
      La&ubreve; grava maniero: 
      <ekz>
        mi estas <tld/>e vundita
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 35:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        se mi jam tiel longe ku&scirc;is, ne estas ja <tld/>e ku&scirc;i ankora&ubreve; iom pli
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>!
      </ekz>
      <ekz>
        por li estis <tld/>e havi la lastan vorton
        <fnt>
          <bib>Fab3</bib>
          <lok>Io</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e maloportuna
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista en Washington en la 15a de a&ubreve;gusto
          1910</vrk>
        </fnt>;
      </ekz>
      <ekz>
        ne <tld/>e, ne <tld/>e, diris <nom>Mefres</nom>
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>.
      </ekz>
    </dif> 
    <refgrp tip="vid">
      <ref cel="fort.0e.multe">forte<sncref/></ref>,
      <ref cel="mult.0e">multe</ref>,
      <ref cel="tre.0">tre</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be"> 
    <trd>&c_v;&c_a;&c_zh;&c_n;&c_a;</trd>, 
    <trd>&c_z;&c_n;&c_a;&c_ch;&c_n;&c_a;</trd>, 
    <trd>&c_ib;&c_s;&c_t;&c_o;&c_t;&c_n;&c_a;</trd>, 
    <trd>&c_s;&c_u;&c_r;'&c_jo;&c_z;&c_n;&c_a;</trd> 
  </trdgrp>
  <trdgrp lng="cs">
    <trd>d&uring;le&zcaron;it&ecaron;</trd>,
    <trd>t&ecaron;&zcaron;ce</trd>,
    <trd>v&aacute;&zcaron;n&ecaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wichtig</trd>,
    <trd>bedeutend</trd>,
    <trd>gravierend</trd>
  </trdgrp>
  <trd lng="fr">gravement</trd>
  <trdgrp lng="hu"> 
    <trd>fontosan</trd>, 
    <trd>s&uacute;lyosan</trd>, 
    <trd>komolyan</trd> 
  </trdgrp>
  <trdgrp lng="pl"> 
    <trd>wa&zdot;nie</trd>, 
    <trd>istotnie</trd>, 
    <trd>znacznie</trd>, 
    <trd>znacz&aogonek;co</trd>, 
    <trd>donio&sacute;le</trd>, 
    <trd>serio<klr> (pot.)</klr></trd> 
  </trdgrp>
  <trd lng="pt">gravemente</trd>
  <trdgrp lng="ru"> 
    <trd>&c_v;&c_a;&c_zh;&c_n;&c_o;</trd>, 
    <trd>&c_z;&c_n;&c_a;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;
    </trd>, 
    <trd>&c_s;&c_je;&c_r;&c_mol;&c_jo;&c_z;&c_n;&c_o;</trd> 
  </trdgrp>
  <trdgrp lng="sk">
    <trd>d&ocirc;le&zcaron;ito</trd>,
    <trd>v&aacute;&zcaron;ne</trd>
  </trdgrp>
  <trd lng="sv">viktigt</trd>
</drv> 

<drv mrk="grav.0i"> 
  <kap><tld/>i</kap> 
  <gra><vspec>ntr</vspec></gra> 
  <snc mrk="grav.0i.KOMUNE"> 
    <dif> 
      Esti <tld/>a: 
      <ekz> 
        <ind>sed tio <mll tip="fin">ne <tld/>as</mll></ind>! 
        <trd lng="de">aber das <mll tip="fin"><ind>macht 
        nichts</ind></mll></trd> 
        <trdgrp lng="fr"> 
          <trd>mais <mll tip="fin">&ccedil;a ne fait rien</mll></trd>, 
          <trd>mais <mll tip="fin">peu importe</mll></trd> 
        </trdgrp> 
        <trd lng="hu">de ez <mll tip="fin"> 
        <ind>egy&aacute;ltal&aacute;n nem 
        fontos</ind></mll></trd> 
        <trd lng="nl">maar <mll tip="fin">dat is niet erg</mll></trd> 
      </ekz>
      <ekz>
        ne <tld/>as, ne <tld/>as, respondis la etulo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio &ccirc;i neniel <tld/>as
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="http://steloj.de/esperanto/butiko">La Stranga Butiko</url></vrk>
          <lok>&Scirc;arado</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sumeto ne tre <tld/>as
        <fnt>
          <bib>Tor</bib>
        </fnt>;
      </ekz>
      <ekz>
        por tiuj, kiuj suferis la militon, <tld/>as haltigi &gcirc;in
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006811.html">Reago: Nekredeblaj
          malveroj</url></vrk>
        </fnt>.
      </ekz>
    </dif> 
  </snc>
  <trdgrp lng="be"> 
    <trd>&c_b;&c_y;&c_c;&c_mol; &c_v;&c_a;&c_zh;&c_n;&c_y;&c_m;</trd>, 
    <trd>&c_z;&c_n;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd> 
  </trdgrp>
  <trd lng="cs">b&yacute;t d&uring;le&zcaron;it&yacute;</trd>
  <trdgrp lng="de"> 
    <trd>wichtig sein</trd>, 
    <trd>bedeutend sein</trd> 
  </trdgrp>
  <trd lng="en">matter</trd>
  <trd lng="es">importar</trd>
  <trdgrp lng="fr"> 
    <trd>importer</trd>, 
    <trd>compter<klr tip="ind"> (importer)</klr></trd>, 
    <trd>convenir<klr tip="ind"> (de)</klr></trd>, 
    <trd>falloir</trd>, 
    <trd>&ecirc;tre <ind>grave</ind></trd>, 
    <trd>peser<klr tip="ind"> (importer)</klr></trd> 
  </trdgrp>
  <trd lng="hu">fontos</trd>
  <trdgrp lng="nl"> 
    <trd>belangrijk zijn</trd>, 
    <trd>gewichtig zijn</trd> 
  </trdgrp>
  <trdgrp lng="pl"> 
    <trd>by&cacute; wa&zdot;nym</trd>, 
    <trd>by&cacute; istotnym</trd>, 
    <trd>by&cacute; znacznym</trd>, 
    <trd>by&cacute; znacz&aogonek;cym</trd>, 
    <trd>mie&cacute; znaczenie</trd>, 
    <trd>by&cacute; donios&lstroke;e</trd>, 
    <trd>by&cacute; serio<klr> (pot.)</klr></trd> 
  </trdgrp>
  <trdgrp lng="pt"> 
    <trd>importar<klr tip="amb"> (ter valor)</klr></trd>, 
    <trd>ser <ind>importante</ind></trd> 
  </trdgrp>
  <trdgrp lng="ru"> 
    <trd>&c_b;&c_y;&c_t;&c_mol;
    <ind>&c_v;&c_a;&c_zh;&c_n;&c_y;&c_m;</ind></trd>, 
    <trd>&c_i;&c_m;&c_je;&c_t;&c_mol;
    <ind>&c_z;&c_n;&c_a;&c_ch;&c_je;&c_n;&c_i;&c_je;</ind></trd> 
  </trdgrp>
  <trd lng="sk">by&tcaron; d&ocirc;le&zcaron;it&yacute;</trd>
  <trdgrp lng="sv"> 
    <trd>vara <ind>viktig</ind></trd>, 
    <trd>spela <ind>roll</ind></trd> 
  </trdgrp>
</drv> 

<drv mrk="grav.0eco"> 
  <kap><tld/>eco</kap> 
  <snc mrk="grav.0eco.KOMUNE"> 
    <dif> 
      Eco de io <tld/>a:
      <ekz>
        al li tiuj figuroj &scirc;ajnis <klr>[&#x2026;]</klr> havantaj la plej altan <tld/>econ
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        pompa parolado pri la <tld/>eco de l' sciencoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        tute specialan <tld/>econ li atribuis al la historio
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu titolo donis al li <tld/>econ, kiu kvaza&ubreve; plifortigis lin en liaj altaj
        oficoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>.
      </ekz>
    </dif> 
  </snc>
  <trdgrp lng="be"> 
    <trd>&c_v;&c_a;&c_zh;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>, 
    <trd> 
    &c_k;&c_a;&c_sh;&c_t;&c_o;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol; 
    </trd> 
  </trdgrp>
  <trdgrp lng="cs">
    <trd>d&uring;le&zcaron;itost</trd>,
    <trd>z&aacute;va&zcaron;nost</trd>
  </trdgrp>
  <trdgrp lng="de"> 
    <trd>Wichtigkeit</trd>, 
    <trd>Bedeutung</trd> 
  </trdgrp>
  <trd lng="en">importance</trd>
  <trdgrp lng="fr"> 
    <trd>gravit&eacute;</trd>, 
    <trd>importance</trd> 
  </trdgrp>
  <trdgrp lng="hu"> 
    <trd>fontoss&aacute;g</trd>, 
    <trd>jelent&odblac;ss&eacute;g</trd> 
  </trdgrp>
  <trdgrp lng="nl"> 
    <trd>belangrijkheid</trd>, 
    <trd>belang</trd>, 
    <trd>betekenis<klr tip="ind"> (belang)</klr></trd> 
  </trdgrp>
  <trdgrp lng="pl"> 
    <trd>waga</trd>, 
    <trd>istota</trd>, 
    <trd>znaczenia</trd>, 
    <trd>donios&lstroke;o&sacute;&cacute;</trd> 
  </trdgrp>
  <trdgrp lng="pt"> 
    <trd>gravidade<klr tip="ind"> (adj.)</klr></trd>, 
    <trd>import&acirc;ncia<klr tip="ind"> (adj.)</klr></trd> 
  </trdgrp>
  <trdgrp lng="ru"> 
    <trd>&c_v;&c_a;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>, 
    <trd>&c_z;&c_n;&c_a;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd> 
  </trdgrp>
  <trdgrp lng="sk">
    <trd>d&ocirc;le&zcaron;itos&tcaron;</trd>,
    <trd>z&aacute;va&zcaron;nos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="sv"> 
    <trd>viktighet</trd>, 
    <trd>betydelse</trd> 
  </trdgrp>
  <trd lng="tp">suli</trd>
</drv> 

<drv mrk="grav.mal0a"> 
  <kap>mal<tld/>a</kap> 
  <snc mrk="grav.mal0a.KOMUNE"> 
    <dif> 
      Bagatela, ne meritanta atenton:
      <ekz>
        ili fari&gcirc;is kiel herbo de kampo, kiel mal<tld/>a verda&jcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 19:26</lok>
        </fnt>;
      </ekz>
      <ekz>
        trompi lin <klr>[&#x2026;]</klr> en mal<tld/>aj aferoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        li havas mienon, kvaza&ubreve; lia trumpetado estas io <tld/>a, sed &gcirc;i estas tute
        mal<tld/>a
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        se mi e&ccirc; havos la plej mal<tld/>an pozicion, tamen se tio, kion mi faros, estos
        bona, &gcirc;i &ccirc;iam havos ian valoron
        <fnt>
          <bib>Fab3</bib>
          <lok>Io</lok>
        </fnt>;
      </ekz>
      <ekz>
        malgranda, mal<tld/>aspekta dometo
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>.
      </ekz>
    </dif> 
    <ref tip="sin" cel="bagate.0a">bagatela</ref>
    <refgrp tip="vid">
       <ref cel="banal.0a">banala</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be"> 
    <trd>&c_n;&c_ja;&c_v;&c_a;&c_zh;&c_n;&c_y;</trd>, 
    <trd>&c_n;&c_je;&c_ib;&c_s;&c_t;&c_o;&c_t;&c_n;&c_y;</trd>, 
    <trd>&c_n;&c_ja;&c_z;&c_n;&c_a;&c_ch;&c_n;&c_y;</trd> 
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bezv&yacute;znamn&yacute;</trd>,
    <trd>lehk&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de"> 
    <trd>unwichtig</trd>, 
    <trd>bedeutungslos</trd> 
  </trdgrp>
  <trd lng="en">unimportant</trd>
  <trdgrp lng="fr"> 
    <trd>anodin</trd>, 
    <trd>b&eacute;nin</trd>, 
    <trd>sans <ind>importance</ind></trd>, 
    <trd>insignifiant</trd> 
  </trdgrp>
  <trdgrp lng="hu"> 
    <trd>l&eacute;nyegtelen</trd>, 
    <trd>jelent&eacute;ktelen</trd>, 
    <trd>bagatell</trd> 
  </trdgrp>
  <trd lng="nl">onbelangrijk</trd>
