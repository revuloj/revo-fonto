    <trd>istotny</trd>,
    <trd>znaczny</trd>,
    <trd>znacz&aogonek;cy</trd>,
    <trd>donios&lstroke;y</trd>,
    <trd>serio<klr> (pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>grave</trd>,
    <trd>importante</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_a;&c_zh;&c_n;&c_y;&c_j;</trd>,
    <trd>
    &c_z;&c_n;&c_a;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;
    </trd>,
    <trd>&c_s;&c_je;&c_r;&c_mol;&c_jo;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>
    &c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;
    </trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>d&ocirc;le&zcaron;it&yacute;</trd>,
    <trd>pov&aacute;&zcaron;liv&yacute;</trd>,
    <trd>v&aacute;&zcaron;ny</trd>,
    <trd>z&aacute;va&zcaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="sv">viktig</trd>
  <trd lng="tok">suli</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_a;&c_zh;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_z;&#x43D;&c_a;&c_ch;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_z;&#x43D;&c_a;&c_ch;&c_u;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_je;&c_r;&c_j;&#x43E;&c_z;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43F;&#x43E;&c_v;&c_a;&c_zh;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x91CD; <pr>zh&#xF2;ng</pr></trd>,
    <trd>&#x6709;&#x5730;&#x4F4D; <pr>y&#x1D2;ud&#xEC;w&#xE8;i</pr></trd>,
    <trd>&#x91CD;&#x8981; <pr>zh&#x14D;ngy&#xE0;o</pr></trd>,
    <trd>&#x8981; <pr>y&#xE0;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="grav.0e">
  <kap><tld/>e</kap>
  <snc mrk="grav.0e.KOMUNE">
    <dif>
      La&ubreve; grava maniero:
      <ekz>
        mi estas <tld/>e vundita
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 35:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        se mi jam tiel longe ku&scirc;is, ne estas ja <tld/>e ku&scirc;i ankora&ubreve; iom pli
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>!
      </ekz>
      <ekz>
        por li estis <tld/>e havi la lastan vorton
        <fnt>
          <bib>Fab3</bib>
          <lok>Io</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e maloportuna
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista en Washington en la 15a de a&ubreve;gusto
          1910</vrk>
        </fnt>;
      </ekz>
      <ekz>
        ne <tld/>e, ne <tld/>e, diris <nom>Mefres</nom>
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fort.0e.multe">forte<sncref/></ref>,
      <ref cel="mult.0e">multe</ref>,
      <ref cel="tre.0">tre</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_zh;&c_n;&c_a;</trd>,
    <trd>&c_z;&c_n;&c_a;&c_ch;&c_n;&c_a;</trd>,
    <trd>&c_ib;&c_s;&c_t;&c_o;&c_t;&c_n;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_r;'&c_jo;&c_z;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>d&uring;le&zcaron;it&ecaron;</trd>,
    <trd>t&ecaron;&zcaron;ce</trd>,
    <trd>v&aacute;&zcaron;n&ecaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wichtig</trd>,
    <trd>bedeutend</trd>,
    <trd>gravierend</trd>
  </trdgrp>
  <trd lng="fr">gravement</trd>
  <trdgrp lng="hu">
    <trd>fontosan</trd>,
    <trd>s&uacute;lyosan</trd>,
    <trd>komolyan</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3072;&#x3069;&#x304F;</trd>,
    <trd>&#x91CD;&#x304F;  <pr>&#x304A;&#x3082;&#x304F;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wa&zdot;nie</trd>,
    <trd>istotnie</trd>,
    <trd>znacznie</trd>,
    <trd>znacz&aogonek;co</trd>,
    <trd>donio&sacute;le</trd>,
    <trd>serio<klr> (pot.)</klr></trd>
  </trdgrp>
  <trd lng="pt">gravemente</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_a;&c_zh;&c_n;&c_o;</trd>,
    <trd>&c_z;&c_n;&c_a;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;
    </trd>,
    <trd>&c_s;&c_je;&c_r;&c_mol;&c_jo;&c_z;&c_n;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>d&ocirc;le&zcaron;ito</trd>,
    <trd>v&aacute;&zcaron;ne</trd>
  </trdgrp>
  <trd lng="sv">viktigt</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_a;&c_zh;&#x43B;&c_i;&c_v;&#x43E;</trd>,
    <trd>&c_s;&c_je;&c_r;&c_j;&#x43E;&c_z;&#x43D;&#x43E;</trd>,
    <trd>&#x43F;&#x43E;&c_v;&c_a;&c_zh;&#x43D;&#x43E;</trd>
  </trdgrp>
