<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>grav</rad>/a
</kap>
<drv mrk="grav.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Atentinda pro sia granda influo a&ubreve; siaj grandaj sekvoj:
      <ekz>
        <tld/>a persono,  oficisto, okazo, kunveno<fnt>Z</fnt>,
        motivo<fnt>Z</fnt>, decido, malsano, helpo, dokumento;
      </ekz>
      <ekz>
        tiu &ccirc;i <tld/>a tago restos por mi &ccirc;iam memorinda
        <fnt><bib>F</bib>, <lok>Ekzercaro, &para; 41</lok></fnt>;
      </ekz>
      <ekz>
        unueco estas la plej <tld/>a bazo de lingvo internacia<fnt>B</fnt>;
      </ekz>
      <ekz>
        a&ubreve;skultu, &ccirc;ar mi parolos <ind><tld/>a&jcirc;o</ind>n
        <fnt><bib>MT</bib>, <lok>&Sen; 8:6</lok></fnt>.
        <trd lng="fr">chose <ind>importante</ind></trd>
        <trd lng="hu">fontos dolog</trd>
        <trd lng="nl">iets <ind>belangrijks</ind></trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="serioz.0a">serioza</ref>,
      <ref cel="signif">signifa</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_zh;&c_n;&c_y;</trd>, 
    <trd>&c_z;&c_n;&c_a;&c_ch;&c_n;&c_y;</trd>, 
    <trd>&c_ib;&c_s;&c_t;&c_o;&c_t;&c_n;&c_y;</trd>, 
    <trd>&c_s;&c_u;&c_r;'&c_jo;&c_z;&c_n;&c_y;</trd>, 
    <trd>&c_v;&c_a;&c_r;&c_t;&c_y; &c_w;&c_v;&c_a;&c_g;&c_ib;</trd> 
  </trdgrp>
  <trd lng="cs">d&#x16F;le&zcaron;it&yacute;</trd>
  <trd lng="en">important</trd>
  <trd lng="es">importante</trd>
  <trdgrp lng="fr">
    <trd>important</trd>,
    <trd>grave</trd>,
    <trd>gros<klr tip="ind"> (important)</klr></trd>,
    <trd>marquant</trd></trdgrp> 
  <trdgrp lng="hu">
    <trd>fontos</trd>,
    <trd>s&uacute;lyos<klr tip="amb"> (&aacute;tv.)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>belangrijk</trd>,
    <trd>gewichtig</trd>,
    <trd>aanzienlijk</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>grave</trd>, 
   <trd>importante</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_a;&c_zh;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_z;&c_n;&c_a;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_je;&c_r;&c_mol;&c_jo;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">viktig</trd>
</drv>

<drv mrk="grav.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      La&ubreve; grava maniero:
      <ekz>
        <tld/>e vundita
        <fnt><bib>MT</bib>, <lok>II. kroniko 35:23</lok></fnt>;
      </ekz>
      <ekz>
        ne <klr>(estas)</klr> <tld/>e!<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_zh;&c_n;&c_a;</trd>, 
    <trd>&c_z;&c_n;&c_a;&c_ch;&c_n;&c_a;</trd>, 
    <trd>&c_ib;&c_s;&c_t;&c_o;&c_t;&c_n;&c_a;</trd>, 
    <trd>&c_s;&c_u;&c_r;'&c_jo;&c_z;&c_n;&c_a;</trd> 
  </trdgrp>
  <trdgrp lng="de">
    <trd>wichtig</trd>,
    <trd>bedeutend</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fontosan</trd>,
    <trd>s&uacute;lyosan</trd>,
    <trd>komolyan</trd>
  </trdgrp>
  <trd lng="pt">gravemente</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_a;&c_zh;&c_n;&c_o;</trd>,
    <trd>&c_z;&c_n;&c_a;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;</trd>,
    <trd>&c_s;&c_je;&c_r;&c_mol;&c_jo;&c_z;&c_n;&c_o;</trd>
  </trdgrp>
  <trd lng="sv">viktigt</trd>
</drv>

<drv mrk="grav.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>a:
      <ekz>
        <ind>sed tio <mll tip="fin">ne <tld/>as</mll></ind>!
        <trd lng="de">aber das <mll tip="fin"><ind>macht nichts</ind></mll></trd>
        <trdgrp lng="fr">
          <trd>mais <mll tip="fin">&ccedil;a ne fait rien</mll></trd>,
          <trd>mais <mll tip="fin">peu importe</mll></trd></trdgrp> 
        <trd lng="hu">de ez <mll tip="fin"><ind>egy&aacute;ltal&aacute;n nem
fontos</ind></mll></trd>
        <trd lng="nl">maar <mll tip="fin">dat is niet erg</mll></trd>
      </ekz>
      <ekz>
        <tld/>as, ke vi mem iru saluti la urbestron.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol; &c_v;&c_a;&c_zh;&c_n;&c_y;&c_m;</trd>, 
    <trd>&c_z;&c_n;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd> 
  </trdgrp>
  <trd lng="es">importar</trd>
  <trdgrp lng="de">
    <trd>wichtig sein</trd>,
    <trd>bedeutend sein</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>importer</trd>,
    <trd>compter<klr tip="ind"> (importer)</klr></trd>,
    <trd>convenir<klr tip="ind"> (de)</klr></trd>,
    <trd>falloir</trd>,
    <trd>&ecirc;tre <ind>grave</ind></trd>,
    <trd>peser<klr tip="ind"> (importer)</klr></trd></trdgrp> 
  <trd lng="hu">fontos</trd>
  <trdgrp lng="nl">
    <trd>belangrijk zijn</trd>,
    <trd>gewichtig zijn</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>importar<klr tip="amb"> (ter valor)</klr></trd>, 
   <trd>ser <ind>importante</ind></trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_y;&c_t;&c_mol; &c_v;&c_a;&c_zh;&c_n;&c_y;&c_m;</trd>,
    <trd>&c_i;&c_m;&c_je;&c_t;&c_mol; &c_z;&c_n;&c_a;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>vara <ind>viktig</ind></trd>,
    <trd>spela <ind>roll</ind></trd>
  </trdgrp>
</drv>

<drv mrk="grav.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Eco de io <tld/>a:
      <ekz>
        oni ne povas malgrandigi la <tld/>econ de tiu demando;
      </ekz>
      <ekz>
        vi vidas nun la <tld/>econ de tiu le&gcirc;oprojekto.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_zh;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>, 
    <trd>&c_k;&c_a;&c_sh;&c_t;&c_o;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd> 
  </trdgrp>
  <trdgrp lng="de">
    <trd>Wichtigkeit</trd>,
    <trd>Bedeutung</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gravit&eacute;</trd>,
    <trd>importance</trd></trdgrp> 
  <trdgrp lng="hu">
    <trd>fontoss&aacute;g</trd>,
    <trd>jelent&odblac;ss&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>belangrijkheid</trd>,
    <trd>belang</trd>,
    <trd>betekenis<klr tip="ind"> (belang)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>gravidade<klr tip="ind"> (adj.)</klr></trd>, 
   <trd>import&acirc;ncia<klr tip="ind"> (adj.)</klr></trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_a;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_n;&c_a;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>viktighet</trd>,
    <trd>betydelse</trd>
  </trdgrp>
</drv>

<drv mrk="grav.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Bagatela, ne meritanta atenton.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_v;&c_a;&c_zh;&c_n;&c_y;</trd>, 
    <trd>&c_n;&c_je;&c_ib;&c_s;&c_t;&c_o;&c_t;&c_n;&c_y;</trd>,  
    <trd>&c_n;&c_ja;&c_z;&c_n;&c_a;&c_ch;&c_n;&c_y;</trd> 
  </trdgrp>
  <trdgrp lng="de">
    <trd>unwichtig</trd>,
    <trd>bedeutungslos</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>sans <ind>importance</ind></trd>,
    <trd>anodin</trd>,
    <trd>b&eacute;nin</trd>,
    <trd>insignifiant</trd></trdgrp> 
  <trdgrp lng="hu">
    <trd>l&eacute;nyegtelen</trd>,
    <trd>jelent&eacute;ktelen</trd>,
    <trd>bagatell</trd>
  </trdgrp>
  <trd lng="nl">onbelangrijk</trd>
  <trdgrp lng="pt">
   <trd>sem <ind>import&acirc;ncia</ind></trd>, 
   <trd>f&uacute;til</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_v;&c_a;&c_zh;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_z;&c_n;&c_a;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>oviktig</trd>,
    <trd>betydelsel&ouml;s</trd>
  </trdgrp>
</drv>

<drv mrk="grav.mal0ajxo">
  <kap>mal<tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Ne atentinda bagatela&jcirc;o.
    </dif>
  </snc>
  <adm>
  Cxu "bagatelo", aux "bagatelajxo"? Cetere la difino
  sxajnas pleonasma. <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_d;&c_r;&c_o;&c_b;&c_ja;&c_z;&c_mol;</trd>, 
    <trd>&c_d;&c_r;&c_a;&c_b;&c_n;&c_ib;&c_c;&c_a;</trd>, 
    <trd>&c_p;&c_u;&c_s;&c_mol;&c_c;&c_ja;&c_k;&c_o;&c_v;&c_ib;&c_n;&c_a;</trd> 
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bagatelle</trd>,
    <trd>broutille</trd>
  </trdgrp> 
  <trdgrp lng="hu">
    <trd>potoms&aacute;g</trd>,
    <trd>bagatell dolog</trd>,
    <trd>csek&eacute;lys&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bagatel</trd>,
    <trd>kleinigheid</trd>,
    <trd>futiliteit</trd>,
    <trd>iets <ind>onbelangrijks</ind></trd>
  </trdgrp>
  <trd lng="pt">bagatela</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_l;&c_o;&c_ch;&c_mol;</trd>,
    <trd>&c_p;&c_u;&c_s;&c_t;&c_ja;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="grav.malpli0igi">
  <kap>malpli<tld/>igi</kap>
  <snc>
    <dif>
      Fari malpli <tld/>a:
      <ekz>
        malpli<tld/>igi sian kulpon per pento.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_m;&c_ja;&c_n;&c_sh;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">untertreiben</trd>
  <trd lng="fr">minimiser<klr tip="ind"> (rendre moins important)</klr></trd>
  <trdgrp lng="hu">
    <trd>(el)<ind>bagateliz&aacute;l</ind></trd>,
    <trd>lekicsinyel</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>minimaliseren</trd>,
    <trd>bagatelliseren</trd>,
    <trd>futiliseren</trd>
  </trdgrp>
  <trd lng="pt">atenuar</trd>
</drv>
</art>

