       <ref cel="kurato.0o.POL">kuratoro<sncref/></ref>,
       <ref cel="mecena.0o">mecenato</ref>,
       <ref cel="patron.0o">patrono</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
