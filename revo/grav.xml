<drv mrk="grav.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="grav.0i.KOMUNE">
    <dif>
      Esti <tld/>a:
      <ekz>
        <ind>sed tio <mll tip="fin">ne <tld/>as</mll></ind>!
        <trd lng="de">aber das <mll tip="fin"><ind>macht
        nichts</ind></mll></trd>
        <trdgrp lng="fr">
          <trd>mais <mll tip="fin">&ccedil;a ne fait rien</mll></trd>,
          <trd>mais <mll tip="fin">peu importe</mll></trd>
        </trdgrp>
        <trd lng="hu">de ez <mll tip="fin">
        <ind>egy&aacute;ltal&aacute;n nem
        fontos</ind></mll></trd>
        <trd lng="nl">maar <mll tip="fin">dat is niet erg</mll></trd>
      </ekz>
      <ekz>
        ne <tld/>as, ne <tld/>as, respondis la etulo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio &ccirc;i neniel <tld/>as
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
          <lok>&Scirc;arado</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sumeto ne tre <tld/>as
        <fnt>
          <bib>Tor</bib>
        </fnt>;
      </ekz>
      <ekz>
        por tiuj, kiuj suferis la militon, <tld/>as haltigi &gcirc;in
        <fnt>
	<aut>Zlatko Ti&scaron;ljar</aut>
	<vrk><url ref="&Monato;/2000/006811.php">Nekredeblaj malveroj</url></vrk>
	<bib>Monato</bib>
	<lok>2000/03, p. 12-13</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_v;&c_a;&c_zh;&c_n;&c_y;&c_m;</ind></trd>,
    <trd>&c_z;&c_n;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">b&yacute;t d&uring;le&zcaron;it&yacute;</trd>
  <trdgrp lng="de">
    <trd>wichtig sein</trd>,
    <trd>bedeutend sein</trd>
  </trdgrp>
  <trd lng="en">matter</trd>
  <trd lng="es">importar</trd>
  <trdgrp lng="fr">
    <trd>importer</trd>,
    <trd>compter<klr tip="ind"> (importer)</klr></trd>,
    <trd>convenir<klr tip="ind"> (de)</klr></trd>,
    <trd>falloir</trd>,
    <trd>&ecirc;tre <ind>grave</ind></trd>,
    <trd>peser<klr tip="ind"> (importer)</klr></trd>
  </trdgrp>
  <trd lng="hu">fontos</trd>
  <trd lng="id">penting</trd>
  <trdgrp lng="ja">
    <trd>&#x91CD;&#x5927;&#x3067;&#x3042;&#x308B;  <pr>&#x3058;&#x3085;&#x3046;&#x3060;&#x3044;&#x3067;&#x3042;&#x308B;</pr></trd>,
    <trd>&#x91CD;&#x8981;&#x3067;&#x3042;&#x308B;  <pr>&#x3058;&#x3085;&#x3046;&#x3088;&#x3046;&#x3067;&#x3042;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>belangrijk zijn</trd>,
    <trd>gewichtig zijn</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>by&cacute; wa&zdot;nym</trd>,
    <trd>by&cacute; istotnym</trd>,
    <trd>by&cacute; znacznym</trd>,
    <trd>by&cacute; znacz&aogonek;cym</trd>,
    <trd>mie&cacute; znaczenie</trd>,
    <trd>by&cacute; donios&lstroke;e</trd>,
    <trd>by&cacute; serio<klr> (pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>importar<klr tip="amb"> (ter valor)</klr></trd>,
    <trd>ser <ind>importante</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_y;&c_t;&c_mol;
    <ind>&c_v;&c_a;&c_zh;&c_n;&c_y;&c_m;</ind></trd>,
    <trd>&c_i;&c_m;&c_je;&c_t;&c_mol;
    <ind>&c_z;&c_n;&c_a;&c_ch;&c_je;&c_n;&c_i;&c_je;</ind></trd>
  </trdgrp>
  <trd lng="sk">by&tcaron; d&ocirc;le&zcaron;it&yacute;</trd>
  <trdgrp lng="sv">
    <trd>vara <ind>viktig</ind></trd>,
    <trd>spela <ind>roll</ind></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_a;&c_t;&c_i; &c_z;&#x43D;&c_a;&c_ch;&c_je;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_b;&c_u;&c_t;&c_i; &c_v;&c_a;&c_zh;&#x43B;&c_i;&c_v;&c_i;&#x43C;</trd>
  </trdgrp>
</drv>

<drv mrk="grav.0igi">
  <kap>
    <tld/>igi, <var><kap>pli<tld/>igi</kap></var>
  </kap>
  <snc>
    <dif>
      Igi pli <tld/>a, prezenti kiel <tld/>a:
      <ekz>
        la malfeli&ccirc;a virino estas enprofundigita en doloron, <klr>[&#x2026;]</klr> kiun hiera&ubreve;
        via &ccirc;eesto ankora&ubreve; pli<tld/>igis
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        la publiko <klr>[&#x2026;]</klr> per sia konduto <tld/>igas a&ubreve; mal<tld/>igas la
        esprimitajn ideojn
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        la diferencon <klr>[inter ili]</klr> <tld/>igas ne nur aparta parolo, sed
        konkura kredo
        <fnt>
          <bib>VivZam</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akcent.0i.KOMUNE">akcenti<sncref/></ref>,
      <ref cel="star.el0igi">elstarigi</ref>,
      <ref cel="emfaz.0i.FIG">emfazi<sncref/></ref>,
<ref cel="fort.0igi">plifortigi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; <ind>&c_z;&c_n;&c_a;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</ind></trd>,
    <trd>&c_p;&c_a;&c_d;&c_k;&c_r;&c_e;&c_s;&c_mol;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol; <ind>&c_v;&c_a;&c_zh;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verst&auml;rken</trd>
  </trdgrp>
  <trd lng="id"><baz>penting</baz>mementingkan</trd>
  <trdgrp lng="ja">
    <trd>&#x3044;&#x3063;&#x305D;&#x3046;&#x91CD;&#x5927;&#x5316;&#x3059;&#x308B;  <pr>&#x3044;&#x3063;&#x305D;&#x3046;&#x3058;&#x3085;&#x3046;&#x3060;&#x3044;&#x304B;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x3088;&#x308A;&#x6DF1;&#x523B;&#x5316;&#x3059;&#x308B; 
    <pr>&#x3088;&#x308A;&#x3057;&#x3093;&#x3053;&#x304F;&#x304B;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_a;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &c_z;&#x43D;&c_a;&c_ch;&c_je;&#x43D;&#x43D;&#x44F;</trd>
