  </trdgrp>
  <trdgrp lng="de">
    <trd>wichtig</trd>,
    <trd>bedeutend</trd>
  </trdgrp>
  <trd lng="en">important</trd>
  <trd lng="es">importante</trd>
  <trdgrp lng="fr">
