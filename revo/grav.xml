<drv mrk="grav.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="grav.0a.KOMUNE">
    <dif>
      Atentinda pro sia granda influo a&ubreve; siaj grandaj sekvoj:
      <ekz>
        ili foriris de li, lasinte lin en <tld/>a malsano
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 24:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        la plej <tld/>aj pleja&gcirc;uloj
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 29:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> staris &ccirc;irka&ubreve;e, akuzante lin pri <klr>[&#x2026;]</klr> <tld/>aj kulpoj
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 25:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        la plej <tld/>aj krimuloj
        <fnt>
          <bib>Fab2</bib>
          <lok>Bildo el kastelremparo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la homo estis tre ri&ccirc;e vestita, li estis videble <tld/>a persono
        <fnt>
          <bib>Fab2</bib>
          <lok>La ombro</lok>
        </fnt>;
      </ekz>
      <ekz>
        li dancas belege, <klr>[&#x2026;]</klr> sed egale <tld/>a demando estas, &ccirc;u li estas
        sufi&ccirc;e instruita
        <fnt>
          <bib>Fab2</bib>
          <lok>La ombro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a festotago
        <fnt>
          <bib>Fab1</bib>
          <lok>Lekanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        estis konfirmado <klr>[&#x2026;]</klr>, tio estis ja <tld/>a tago por ili, el infanoj ili subite
        fari&gcirc;is plenkreskaj
        homoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Sonorilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        a&ubreve;skultu, &ccirc;ar mi parolos <ind><tld/>a&jcirc;o</ind>n
        <fnt><bib>MT</bib>, <lok>&Sen; 8:6</lok></fnt>;
        <trd lng="fr">chose <ind>importante</ind></trd>
        <trd lng="hu">fontos dolog</trd>
        <trd lng="id">hal <ind>penting</ind></trd>
        <trd lng="nl">iets <ind>belangrijks</ind></trd>
      </ekz>
      <ekz>
        ofte de ka&ubreve;zo senenhava venas efiko plej <tld/>a
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pez.0a.multe0a">peza<sncref/></ref>,
      <ref cel="serioz.0a">serioza</ref>,
      <ref cel="signif.0a.FIG">signifa</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_zh;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_n;&c_a;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_ib;&c_s;&c_t;&c_o;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_u;&c_r;'&c_jo;&c_z;&c_n;&c_y;</trd>,
    <trd>&c_v;&c_a;&c_r;&c_t;&c_y; &c_w;&c_v;&c_a;&c_g;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_v;&c_a;&c_zh;&c_je;&c_n;</trd>,
    <trd>&c_s;&c_malmol;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;</trd>
  </trdgrp>
  <trd lng="bo">&t_ga;&t_la;&t_tsheg;&t_cha;&t_drengbu;&t_na;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>d&uring;le&zcaron;it&yacute;</trd>,
    <trd>d&uring;le&zcaron;it&yacute;</trd>,
    <trd>hrub&yacute; (v&aacute;&zcaron;n&yacute;)</trd>,
    <trd>pov&aacute;&zcaron;liv&yacute;</trd>,
