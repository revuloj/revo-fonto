<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mucid.xml,v 1.26 2025/12/05 16:39:29 revo Exp $">
<kap>
  <rad>mucid</rad>/a
  <fnt><bib>JED</bib></fnt>
</kap>

<drv mrk="mucid.0a">
  <kap><tld/>a</kap>
  <snc mrk="mucid.0a.odora">
    <dif>
      Havanta &scirc;iman odoron a&ubreve; guston:
      <ekz>
        <tld/>a klaso&ccirc;ambro fermita jam de du monatoj
        <fnt>
          <aut>A. Fournier, trad. R. Bernard</aut>,
          <vrk>La Granda Meaulnes</vrk>,
          <lok>p. 198</lok>
        </fnt>;
      </ekz>
      <ekz>
        meze de mallumo, malsekeco, <tld/>a aero, inter konsumi&gcirc;intaj, mortantaj
        viza&gcirc;oj, &scirc;i kudradis
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la homamaso parfumis je vino, a&ccirc;a kosmetiko kaj <tld/>o
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="haladz.0a">haladza</ref>,
      <ref cel="fresx.mal0a.KOMUNE">malfre&scirc;a<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_t;&c_h;&c_l;&c_y;</trd>,
      <trd>&c_z; &c_p;&c_a;&c_h;&c_a;&c_m; <ind>&c_p;&c_l;&c_je;&c_s;&c_je;&c_n;&c_ib;</ind></trd>
    </trdgrp>
    <trd lng="bg">&c_m;&c_u;&c_h;&c_l;&c_ja;&c_s;&c_a;&c_l;</trd>
    <trdgrp lng="de">
      <trd>muffig</trd>,
      <trd>abgestanden</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>mucid</trd>,
      <trd>musty</trd>,
      <trd>moldy</trd>,
      <trd>foul</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>qui sent le <ind>moisi</ind></trd>,
      <trd>qui sent le <ind>renferm&eacute;</ind></trd>,
      <trd>moisi<klr> (odeur)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>dohos</trd>,
      <trd>pen&eacute;szszag&uacute;</trd>,
      <trd>&aacute;porodott</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>muf</trd>,
      <trd>duf</trd>
    </trdgrp>
    <trd lng="pl">st&eogonek;ch&lstroke;y</trd>
    <trdgrp lng="pt">
      <trd>bolorento</trd>,
      <trd>mofado</trd>,
      <trd>ran&ccedil;oso</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_t;&c_h;&c_l;&c_y;&c_j;</trd>,
      <trd>&c_p;&c_a;&c_h;&c_n;&c_u;&c_shch;&c_i;&c_j; <ind>&c_p;&c_l;&c_je;&c_s;&c_je;&c_n;&c_mol;&c_ju;</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="mucid.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malagrable <ref cel="mod.eks0a">eksmoda</ref>, <ref tip="sin" cel="fresx.mal0a.FIG">malfre&scirc;a</ref>:
      <ekz>
        ili super<klr>[as je]</klr> mensa limigiteco la plej <tld/>an kaj polvokovritan
        prusan imperiestran oficistan cerbeton
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 38</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj <tld/>aj &scirc;ercoj verkitaj a&ubreve; tradukitaj de tri
        &leftquot;renomaj&rightquot; a&ubreve;toroj
        <fnt><aut>Paul Veenman</aut>: <vrk>Diverskvalita erotiko
        (Antologio de amoro)</vrk>, <bib>Monato</bib><lok>jaro 1996a,
        numero 4a, p. 25a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kaduk.0a.homo">kaduka<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_p;&c_l;&c_je;&c_s;&c_mol;&c_n;&c_ja;&c_v;&c_je;&c_l;&c_y; <klr tip="amb">(&c_p;&c_je;&c_r;&c_a;&c_n;.)</klr></trd>,
      <trd>&c_s;&c_a;&c_s;&c_t;&c_a;&c_r;&c_e;&c_l;&c_y;</trd>
    </trdgrp>
    <trd lng="bg">&c_o;&c_s;&c_t;&c_a;&c_r;&c_ja;&c_l;</trd>
    <trdgrp lng="de">
      <trd>altmodisch</trd>,
      <trd>verstaubt</trd>,
      <trd>&uuml;berkommmen</trd>
    </trdgrp>
    <trd lng="en">musty</trd>
    <trdgrp lng="fr">
      <trd>de <ind>mauvais go&ucirc;t</ind></trd>,
      <trd>&eacute;cul&eacute;<klr> (pass&eacute; de mode)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&aacute;satag</trd>,
      <trd>idej&eacute;tm&uacute;lt</trd>
    </trdgrp>
    <trd lng="nl">stoffig</trd>
    <trd lng="pl">nie&sacute;wie&zdot;y</trd>
    <trdgrp lng="pt">
      <trd>bolorento</trd>,
      <trd>mofado</trd>,
      <trd>ran&ccedil;oso</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_p;&c_l;&c_je;&c_s;&c_n;&c_je;&c_v;&c_je;&c_l;&c_y;&c_j;<klr tip="amb">
      (&c_p;&c_je;&c_r;&c_je;&c_n;.)</klr></trd>,
      <trd>&c_i;&c_z;&c_b;&c_i;&c_t;&c_y;&c_j;<klr tip="amb"> (&c_o; &c_sh;&c_u;&c_t;&c_k;&c_je; &c_i;
      &c_t;.&c_p;.)</klr></trd>,
      <trd>&c_s; <ind>&c_b;&c_o;&c_r;&c_o;&c_d;&c_o;&c_j;</ind><klr tip="amb"> (&c_o; &c_sh;&c_u;&c_t;&c_k;&c_je; &c_i;
      &c_t;.&c_p;.)</klr></trd>
    </trdgrp>
    <trdgrp lng="zh">
      <trd>&#x5E26;&#x9709; <pr>d&agrave;im&eacute;i</pr></trd>,
      <trd>&#x5E36;&#x9709; <pr>d&agrave;im&eacute;i</pr></trd>,
      <trd>&#x4E0D;&#x65F6;&#x9AE6; <pr>b&ugrave;sh&iacute;m&aacute;o</pr></trd>,
      <trd>&#x4E0D;&#x6642;&#x9AE6; <pr>b&ugrave;sh&iacute;m&aacute;o</pr></trd>,
      <trd>&#x8001;&#x571F; <pr>l&#x1CE;ot&#x1D4;</pr></trd>
    </trdgrp>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>&scirc;imodora</ke>, <ke>&scirc;imgusta</ke>.
  </rim>

  <trdgrp lng="cs">
    <trd>zatuchl&yacute;</trd>,
    <trd>ztuchl&yacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x304B;&#x3073;&#x304F;&#x3055;&#x3044;</trd>,
    <trd>&#x304B;&#x3073;&#x306E;&#x751F;&#x3048;&#x305F;  <pr>&#x304B;&#x3073;&#x306E;&#x306F;&#x3048;&#x305F;</pr></trd>
  </trdgrp>
  <trd lng="ro">muced</trd>
  <trd lng="sk">stuchnut&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_t;&c_h;&c_l;&c_i;&c_j;</trd>,
    <trd>&c_t;&c_u;&c_h;&c_l;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="mucid.0i">
  <kap>
    <tld/>i
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="odor.0i">Odori</ref> &scirc;ime, malfre&scirc;e:
      <ekz>
        la aero <tld/>is kaj pezis
        <fnt>
          <bib>Monato</bib>
          <aut>Julian Modest</aut>
          <vrk><url ref="&Monato;/2011/010260.php"
              >Labirinto en &ccirc;ina lo&gcirc;kvartalo</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">m&uuml;ffeln</trd>
</drv>

</art>
