<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kribr.xml,v 1.35 2024/01/21 12:37:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kribr</rad>/i
  <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="kribr.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Apartigi per multetrua instrumento la dikajn partojn de solidaj
      materioj for de la pli maldikaj a&ubreve; pli subtilaj partoj:
      <ekz>
        <tld/>i oron el sablo;
      </ekz>
      <ekz>
        mi bezonas muelilon por mueli &gcirc;in, <tld/>ilon por kribri &gcirc;in, kaj fermentilon
        por fermenti &gcirc;in, por fari panon el <klr>[la greno]</klr>
        <fnt>
          <bib>Rob</bib>
          <lok>Parto III</lok>
        </fnt>;
      </ekz>
      <ekz>
        la maldensaj flavi&gcirc;antaj frondaroj de la tombejo <tld/>is palan a&ubreve;tunan sunon
        <fnt>
          <bib>Kiso</bib>
          <lok>Tombeja Arabesko</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="filtr.0i">filtri</ref>
    </refgrp>
  </snc>
  <snc mrk="kribr.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Metode <ref tip="super" cel="elekt.0i">elekti</ref> deziratajn erojn el
      granda kvanto da neordigita materialo:
      <ekz>
        konservi por postaj esplorantoj tion, kion <klr>[&#8230;]</klr> sesdek
        kunlaborantoj sukcesis <tld/>i el treege neorganizita historia materialo
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#8470; 4 (102)</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u viajn faktojn la&ubreve; ju&gcirc;o
        <fnt>
          <bib>KrM</bib>
          <lok>1. Kiu Sin Enjungis, Devas Tiri.</lok>
        </fnt>;
      </ekz>
      <ekz>
        se oni sciis <tld/>i liajn parolojn, oni povis multon lerni
        <fnt>
          <bib>KrM</bib>
          <lok>22. Per Lerta Admono Alfluas La Mono.</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili estis tro junaj por <tld/>i kaj elekti la dira&jcirc;ojn de la plenkreskuloj
        <fnt>
          <bib>HsT</bib>
          <lok>La nepo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tir.el0i.rezultigi">eltiri<sncref/></ref>,
      <ref cel="ekstra.0i">ekstrakti</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>extrahieren</trd>,
      <trd>aussieben</trd>,
      <trd>ausfiltern</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_s;&c_ja;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_p;&c_r;&c_a;&c_z;&c_mol;
  &c_s;&c_ib;&c_t;&c_a;, &c_r;&c_e;&c_sh;&c_a;&c_t;&c_a;)</klr></trd>
  <trdgrp lng="ca">
    <trd>tamisar</trd>,
    <trd>colar</trd>,
    <trd>cerndre</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>cedit</trd>,
    <trd>pros&iacute;t</trd>,
    <trd>pros&iacute;vat</trd>
  </trdgrp>
  <trd lng="de">sieben</trd>
  <trdgrp lng="en">
    <trd>sift</trd>,
    <trd>riddle</trd>
  </trdgrp>
  <trd lng="es">colar</trd>
  <trd lng="fa">&a_A;&a_l;&f_k; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">tamiser</trd>
  <trdgrp lng="hu">
    <trd>szit&aacute;l</trd>,
    <trd>rost&aacute;l</trd>
  </trdgrp>
  <trd lng="ja">&#12405;&#12427;&#12356;&#12395;&#12363;&#12369;&#12427;</trd>
  <trdgrp lng="nl">
    <trd>zeven<klr tip="ind"> (ziften)</klr></trd>,
    <trd>ziften</trd>
  </trdgrp>
  <trd lng="pl">przesiewa&cacute;</trd>
  <trd lng="pt">peneirar</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_je;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>osieva&tcaron;</trd>,
    <trd>preosieva&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="th">
    <trd>&#3585;&#3619;&#3629;&#3591;</trd>,
    <trd>&#3619;&#3656;&#3629;&#3609;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_ib;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_ib;&c_v;&c_a;&c_t;&c_i; (&c_ch;&c_je;&c_r;&c_je;&c_z; &c_s;&c_i;&c_t;&c_o;</trd>,
    <trd>&c_r;&c_je;&c_sh;&c_je;&c_t;&c_o;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#31579; <pr>sh&amacron;i</pr></trd>,
    <trd>&#31721; <pr>sh&amacron;i</pr></trd>,
    <trd>&#32599; <pr>lu&oacute;</pr></trd>,
    <trd>&#32645; <pr>lu&oacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kribr.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Esti <tld/>ita:
      <ekz>
        la ne&gcirc;o <tld/>i&gcirc;is al li kvaza&ubreve; dum ne&gcirc;tempesto
        <fnt>
          <bib>VRA</bib>
          <lok>Albert Viksten</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la pluvo malrapide komencis <tld/>i&gcirc;i tra la nebulo
        <fnt>
          <bib>VRA</bib>
          <lok>Lars Ahlin</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_s;&c_ja;&c_v;&c_a;&c_c;&c_c;&c_a;
  <klr>(&c_p;&c_r;&c_a;&c_z;&c_mol; &c_s;&c_ib;&c_t;&c_a;,
  &c_r;&c_e;&c_sh;&c_a;&c_t;&c_a;)</klr></trd>
  <trdgrp lng="ca">
    <trd>cerndre's</trd>,
    <trd>colar-se</trd>
  </trdgrp>
  <trd lng="de"><ind>gesiebt</ind> werden</trd>
  <trdgrp lng="hu">
    <trd>szit&aacute;l&oacute;dik</trd>,
    <trd>kirost&aacute;l&oacute;dik</trd>,
    <trd>megsz&udblac;r&odblac;dik</trd>,
  </trdgrp>
  <trd lng="es">colarse</trd>
  <trd lng="ja">&#12405;&#12427;&#12356;&#12395;&#12363;&#12363;&#12427;</trd>
  <trdgrp lng="nl">
    <trd>gezeefd worden</trd>,
    <trd>gezift worden</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przesiewa&cacute; si&eogonek;</trd>,
    <trd>torowa&cacute; sobie drog&eogonek;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_je;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="kribr.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Multetrua instrumento por <tld/>i:
      <ekz>
        <tld/>ilo de muskato
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        lavi berojn en <tld/>ilo;
      </ekz>
      <ekz>
        mi devas fari <tld/>ilon por senrubigi la grenon
        <fnt>
          <bib>Rob</bib>
          <lok>Parto III</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> azenoj <klr>[&#8230;]</klr> man&gcirc;os miksitan nutra&jcirc;on, &scirc;utitan per
        <tld/>ilo kaj &scirc;ovelilo
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 30:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        percepti &ccirc;iun lingvon tra la fonema <tld/>ilo de la propra gepatra lingvo
        <fnt>
          <bib>LAE</bib>
          <lok>2. Fonetiko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>&ccirc;erpi akvon per <tld/>ilo</ind>
        <klr>(<ref cel="van.0e">vane</ref> labori)</klr>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="hu">rost&aacute;val hordja a vizet</trd>
        <trd lng="pl">czerpa&cacute; wod&eogonek; sitem</trd>
        <trd lng="ru">&c_n;&c_o;&c_s;&c_i;&c_t;&c_mol; &c_v;&c_o;&c_d;&c_u;
        &c_v; &c_r;&c_je;&c_sh;&c_je;&c_t;&c_je;</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_sh;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_s;&c_ib;&c_t;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>garbell</trd>,
    <trd>sed&agrave;s</trd>,
    <trd>colador</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>cedn&iacute;k</trd>,
    <trd>s&iacute;to</trd>,
    <trd>&rcaron;e&scaron;eto</trd>
  </trdgrp>
  <trd lng="de">Sieb</trd>
  <trdgrp lng="en">
    <trd>seive</trd>,
    <trd>riddle</trd>,
    <trd>strainer</trd>,
    <trd>sifter</trd>,
    <trd>colander</trd>
  </trdgrp>
  <trd lng="es">colador</trd>
  <trd lng="fa">&a_A;&a_l;&f_k;</trd>
  <trdgrp lng="fr">
    <trd>tamis</trd>, <trd>passoire</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szita</trd>,
    <trd>rosta</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12405;&#12427;&#12356;</trd>,
    <trd>&#36984;&#21029;&#27231;  <pr>&#12379;&#12435;&#12409;&#12388;&#12365;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zeef</trd>,
    <trd>zift</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>sito</trd>,
    <trd>przetak</trd>,
    <trd>rzeszoto</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_i;&c_t;&c_o;</trd>,
    <trd>&c_r;&c_je;&c_sh;&c_je;&c_t;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>cedidlo</trd>,
    <trd>rie&ccaron;ica</trd>,
    <trd>sito</trd>
  </trdgrp>
<trdgrp lng="th">
    <trd>&#3607;&#3637;&#3656;&#3585;&#3619;&#3629;&#3591;</trd>,
    <trd>&#3585;&#3619;&#3632;&#3594;&#3629;&#3609;</trd>,
    <trd>&#3605;&#3632;&#3649;&#3585;&#3619;&#3591;&#3585;&#3619;&#3629;&#3591;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&c_t;&c_o;</trd>,
    <trd>&c_r;&c_je;&c_sh;&c_je;&c_t;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#31579; <pr>sh&amacron;i</pr></trd>,
    <trd>&#31721; <pr>sh&amacron;i</pr></trd>,
    <trd>&#28431;&#37707; <pr>l&ograve;ugu&omacron;</pr></trd>,
    <trd>&#28431;&#38149; <pr>l&ograve;ugu&omacron;</pr></trd>,
    <trd>&#31728; <pr>b&ugrave;</pr></trd>
  </trdgrp>
</drv>
</art>
