<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kribr.xml,v 1.31 2022/03/25 07:40:27 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kribr</rad>/i
</kap>
<drv mrk="kribr.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Apartigi per multetrua instrumento la dikajn partojn de solidaj
      materioj for de la pli maldikaj a&ubreve; pli subtilaj partoj:
      <ekz>
        <tld/>i sablon, grenon, farunon;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i kaj senbalastigi la lingvon.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="filtr.0i">filtri</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_s;&c_ja;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_p;&c_r;&c_a;&c_z;&c_mol;
  &c_s;&c_ib;&c_t;&c_a;, &c_r;&c_e;&c_sh;&c_a;&c_t;&c_a;)</klr></trd>
  <trdgrp lng="ca">
    <trd>tamisar</trd>,
    <trd>colar</trd>,
    <trd>cerndre</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>cedit</trd>,
    <trd>pros&iacute;t</trd>,
    <trd>pros&iacute;vat</trd>
  </trdgrp>
  <trd lng="de">sieben</trd>
  <trdgrp lng="en">
    <trd>sift</trd>,
    <trd>riddle</trd>
  </trdgrp>
  <trd lng="es">colar</trd>
  <trd lng="fa">&a_A;&a_l;&f_k; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">tamiser</trd>
  <trdgrp lng="hu">
    <trd>szit&aacute;l</trd>,
    <trd>rost&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zeven<klr tip="ind"> (ziften)</klr></trd>,
    <trd>ziften</trd>
  </trdgrp>
  <trd lng="pl">przesiewa&cacute;</trd>
  <trd lng="pt">peneirar</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_je;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>osieva&tcaron;</trd>,
    <trd>preosieva&tcaron;</trd>
  </trdgrp>
<trdgrp lng="th">
    <trd>&#3585;&#3619;&#3629;&#3591;</trd>,
    <trd>&#3619;&#3656;&#3629;&#3609;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_ib;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_ib;&c_v;&c_a;&c_t;&c_i; (&c_ch;&c_je;&c_r;&c_je;&c_z; &c_s;&c_i;&c_t;&c_o;</trd>,
    <trd>&c_r;&c_je;&c_sh;&c_je;&c_t;&c_o;)</trd>
  </trdgrp>
</drv>

<drv mrk="kribr.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Esti <tld/>ita:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la sunaj radioj <tld/>i&gcirc;is tra la folioj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_s;&c_ja;&c_v;&c_a;&c_c;&c_c;&c_a;
  <klr>(&c_p;&c_r;&c_a;&c_z;&c_mol; &c_s;&c_ib;&c_t;&c_a;,
  &c_r;&c_e;&c_sh;&c_a;&c_t;&c_a;)</klr></trd>
  <trdgrp lng="ca">
    <trd>cerndre's</trd>,
    <trd>colar-se</trd>
  </trdgrp>
  <trd lng="de">gesiebt werden</trd>
  <trdgrp lng="hu">
    <trd>szit&aacute;l&oacute;dik</trd>,
    <trd>kirost&aacute;l&oacute;dik</trd>,
    <trd>megsz&udblac;r&odblac;dik</trd>,
  </trdgrp>
  <trd lng="es">colarse</trd>
  <trdgrp lng="nl">
    <trd>gezeefd worden</trd>,
    <trd>gezift worden</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przesiewa&cacute; si&eogonek;</trd>,
    <trd>torowa&cacute; sobie drog&eogonek;</trd>
  </trdgrp>  
  <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_je;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="kribr.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Multetrua instrumento por <tld/>i:
      <ekz>
        <ind>&ccirc;erpi akvon per <tld/>ilo</ind>
        <klr>(vane labori)</klr><fnt><bib>PrV</bib></fnt>.
        <trd lng="hu">rost&aacute;val hordja a vizet</trd>
        <trd lng="pl">czerpa&cacute; wod&eogonek; sitem</trd>
        <trd lng="ru">&c_n;&c_o;&c_s;&c_i;&c_t;&c_mol; &c_v;&c_o;&c_d;&c_u; 
        &c_v; &c_r;&c_je;&c_sh;&c_je;&c_t;&c_je;</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="van.0e">vane</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_sh;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_s;&c_ib;&c_t;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>garbell</trd>,
    <trd>sed&agrave;s</trd>,
    <trd>colador</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>cedn&iacute;k</trd>,
    <trd>s&iacute;to</trd>,
    <trd>&rcaron;e&scaron;eto</trd>
  </trdgrp>
  <trd lng="de">Sieb</trd>
  <trdgrp lng="en">
    <trd>seive</trd>,
    <trd>riddle</trd>,
    <trd>strainer</trd>,
    <trd>sifter</trd>,
    <trd>colander</trd>
  </trdgrp>
  <trd lng="es">colador</trd>
  <trd lng="fa">&a_A;&a_l;&f_k;</trd>
  <trdgrp lng="fr">
    <trd>tamis</trd>, <trd>passoire</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szita</trd>,
    <trd>rosta</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zeef</trd>,
    <trd>zift</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>sito</trd>,
    <trd>przetak</trd>,
    <trd>rzeszoto</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_i;&c_t;&c_o;</trd>,
    <trd>&c_r;&c_je;&c_sh;&c_je;&c_t;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>cedidlo</trd>,
    <trd>rie&ccaron;ica</trd>,
    <trd>sito</trd>
  </trdgrp>
<trdgrp lng="th">
    <trd>&#3607;&#3637;&#3656;&#3585;&#3619;&#3629;&#3591;</trd>,
    <trd>&#3585;&#3619;&#3632;&#3594;&#3629;&#3609;</trd>,
    <trd>&#3605;&#3632;&#3649;&#3585;&#3619;&#3591;&#3585;&#3619;&#3629;&#3591;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&c_t;&c_o;</trd>,
    <trd>&c_r;&c_je;&c_sh;&c_je;&c_t;&c_o;</trd>
  </trdgrp>
</drv>
</art>
