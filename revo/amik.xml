<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>amik</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="amik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="amik.0o.KOMUNE">
    <dif>
      Persono, amata kaj amanta, kiu estas korligita kun alia pro 
      inklino sed ne pro seksamo, nek pro familianeco:
      <ekz>
        ne ripro&ccirc;u vian <tld/>on
        <fnt><bib>F</bib>, <lok>&FE; 22</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>o en &gcirc;ojo kaj ploro<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind><tld/>on montras malfeli&ccirc;o</ind><fnt><bib>PrV</bib>
        </fnt>.
        <trd lng="hu">bajban ismerszik meg, ki a j&oacute; 
        <ind>bar&aacute;t</ind></trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_ja;&c_b;&c_r;&c_a;</trd>,
    <trd>&c_d;&c_r;&c_u;&c_g;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_ja;&c_c;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>keneil</trd>,
    <trd>mignon</trd>
  </trdgrp>
  <trd lng="ca">amic</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;&iacute;tel</trd>,
    <trd>partner</trd>,
    <trd>p&rcaron;&iacute;tel</trd>
  </trdgrp>
  <trd lng="de">Freund</trd>
  <trd lng="en">friend</trd>
  <trdgrp lng="es">
    <trd>amigo <klr>(= vira/malina amiko, viramiko)</klr></trd>,
    <trd>amiga</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_d;&a_w;&a_s;&a_t;</trd>,
    <trd>&f_y;&a_A;&a_r;</trd>,
    <trd>&a_r;&a_f;&f_y;&a_q;</trd>
  </trdgrp>
  <trd lng="fr">ami</trd>
  <trd lng="hu">bar&aacute;t</trd>
  <trdgrp lng="id">
    <trd>kawan</trd>,
    <trd>teman</trd>,
    <trd>sahabat</trd>
  </trdgrp>
  <trd lng="it">amico</trd>
  <trd lng="ku">hewal</trd>
  <trd lng="nl">vriend</trd>
  <trd lng="no">venn</trd>
  <trd lng="oc">amic</trd>
  <trd lng="pl">przyjaciel</trd>
  <trd lng="pt">amigo</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_r;&c_u;&c_g;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_ja;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kamar&aacute;t</trd>,
    <trd>priate&lcaron;</trd>
  </trdgrp>
  <trd lng="sv">v&auml;n</trd>
  <trdgrp lng="tr">
    <trd>arkada&scedil;</trd>,
    <trd>dost</trd>
  </trdgrp>
</drv>

<drv mrk="amik.0a">
  <kap><tld/>a</kap>
  <snc mrk="amik.0a.KOMUNE">
    <dif>
      Propra al la rilatoj inter <tld/>oj:
      <ekz>
        <tld/>a helpo, konsilo, ripro&ccirc;o;
      </ekz>
      <ekz>
        ili iris viziti unue Palliason, kiu bone
        konis ilin amba&ubreve;, kaj kun kiu ili estis en intima, 
        <tld/>a kontakto
        <fnt><bib>SatirRak</bib>, <lok>Pythagoras, 2</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>e via <klr>(konkluda salutfrazo de letero)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">karadek</trd>
  <trdgrp lng="ca">
    <trd>amist&oacute;s</trd>,
    <trd>amical</trd>
  </trdgrp>
  <trd lng="cs">p&rcaron;&aacute;telsk&yacute;</trd>
  <trd lng="de">freundschaftlich</trd>
  <trd lng="en">friendly</trd>
  <trdgrp lng="es">
    <trd>amistoso</trd>,
    <trd>amistosa</trd>
  </trdgrp>
  <trd lng="fa">&a_d;&a_w;&a_s;&a_t;&a_A;&a_n;&a_h;</trd>
  <trd lng="fr">amical</trd>
  <trdgrp lng="hu">
    <trd>bar&aacute;ti</trd>,
    <trd>bar&aacute;ts&aacute;gos</trd>
  </trdgrp>
  <trd lng="it">amichevole</trd>
  <trdgrp lng="nl">
    <trd>vriendelijk</trd>,
    <trd>vriendschappelijk</trd>
  </trdgrp>
  <trd lng="no">vennlig</trd>
  <trdgrp lng="pl">
    <trd>przyjacielski</trd>,
    <trd>przyjazny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_r;&c_u;&c_zh;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>
      &c_d;&c_r;&c_u;&c_zh;&c_je;&c_l;&c_ju;&c_b;&c_n;&c_y;&c_j;
    </trd>,
    <trd>
    &c_d;&c_r;&c_u;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;
    </trd>,
    <trd>
      &c_p;&c_r;&c_i;&c_ja;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_k;&c_i;&c_j;
    </trd>
  </trdgrp>
  <trd lng="sk">priate&lcaron;sk&yacute;</trd>
</drv>

<drv mrk="amik.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="amik.0i.KOMUNE">
    <dif>
      Esti en <tld/>aj rilatoj:
      <ekz>
        kiam la Poeto <tld/>as kun Muzo, &scirc;i kantas per juna,
        pura, bela, gaja, ama vo&ccirc;o
        <fnt>
        <aut>B. Kolker</aut>,
        <vrk>Poeto de amo kaj revolucio</vrk>,
        <bib>Monato</bib>,
        <lok>1996:9, p. 24a</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">p&rcaron;&aacute;telit se</trd>
  <trdgrp lng="de">
    <trd>lieben</trd>,
    <trd>befreundet sein</trd>
  </trdgrp>
  <trd lng="en">to be friends</trd>
  <trdgrp lng="es">
    <trd>ser amigo</trd>,
    <trd>ser amiga</trd>
  </trdgrp>
  <trd lng="fa">&a_d;&a_w;&a_s;&a_t; &a_b;&a_w;&a_d;&a_n;</trd>
  <trdgrp lng="hu">
    <trd>bar&aacute;tkozik</trd>,
    <trd>bar&aacute;ts&aacute;gban van</trd>
  </trdgrp>
  <trd lng="id">[<ind>teman</ind>] berteman</trd>
  <trd lng="it">essere <ind>amico</ind></trd>
  <trd lng="nl">bevriend zijn met</trd>
  <trd lng="pl">przyja&zacute;ni&cacute; si&eogonek;</trd>
  <trd lng="ru">&c_d;&c_r;&c_u;&c_zh;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">priateli&tcaron; sa</trd>
</drv>

<drv mrk="amik.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="amik.0ajxo.KOMUNE">
    <dif>
      <tld/>a ago, montro de <tld/>eco:
      <ekz>
        fari <tld/>a&jcirc;on al iu;
      </ekz>
      <ekz>
        prezentu al via patro miajn <tld/>a&jcirc;ojn.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Freundschaftsdienst</trd>
  <trd lng="en">act of friendship</trd>
  <trdgrp lng="es">
    <trd>acto de amistad</trd>,
    <trd>muestra de amistad</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_l;&a_T;&a_f;</trd>,
    <trd>&a_m;&a_H;&a_b;&a_t;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>amiti&eacute;<klr> (acte amical)</klr></trd>,
    <trd>t&eacute;moignage d&#8217;amiti&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bar&aacute;ti sz&iacute;vess&eacute;g</trd>,
    <trd>bar&aacute;ti gesztus</trd>,
    <trd>bar&aacute;ti megnyilv&aacute;nul&aacute;s</trd>
  </trdgrp>
  <trd lng="it">attestazione d'<ind>amicizia</ind></trd>
  <trd lng="nl">vriendendienst</trd>
  <trd lng="no">vennetjeneste</trd>
  <trdgrp lng="pl">
    <trd>oznaka przyja&zacute;ni</trd>,
    <trd>dow&oacute;d przyja&zacute;ni</trd>,
    <trd>przyjacielska przys&lstroke;uga</trd>,
    <trd>przyjacielskie pozdrowienie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>
    &c_d;&c_r;&c_u;&c_zh;&c_je;&c_s;&c_k;&c_i;&c_j; 
    &c_p;&c_o;&c_s;&c_t;&c_u;&c_p;&c_o;&c_k;
    </trd>,
    <trd>
    &c_p;&c_r;&c_o;&c_ja;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je; 
    <ind>&c_d;&c_r;&c_u;&c_zh;&c_b;&c_y;</ind>
    </trd>,
    <trd>
    &c_z;&c_n;&c_a;&c_k; <ind>&c_d;&c_r;&c_u;&c_zh;&c_b;&c_y;</ind>
    </trd>
  </trdgrp>
</drv>

<drv mrk="amik.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="amik.0eco.KOMUNE">
    <dif>
      Situacio de <tld/>aj rilatoj kaj sentoj inter homoj:
      <ekz>
        mi vivas kun li en granda <tld/>eco
        <fnt><bib>F</bib>, <lok>&FE; 35</lok></fnt>;
      </ekz>
      <ekz>
        ne ekzistas en komerco <tld/>eco nek &scirc;erco
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="br">
    <trd>karantez<klr tip="ind"> (mignoniezh)</klr></trd>,
    <trd>keneiliezh</trd>,
    <trd>mignoniezh</trd>
  </trdgrp>
  <trd lng="ca">amistat</trd>
  <trdgrp lng="cs">
    <trd>dru&zcaron;ba</trd>,
    <trd>p&rcaron;&aacute;telsk&yacute; svazek</trd>,
    <trd>p&rcaron;&aacute;telstv&iacute;</trd>
  </trdgrp>
  <trd lng="de">Freundschaft</trd>
  <trd lng="en">friendship</trd>
  <trd lng="es">amistad</trd>
  <trdgrp lng="fa">
    <trd>&a_d;&a_w;&a_s;&a_t;&f_y;</trd>,
    <trd>&a_r;&a_f;&a_A;&a_q;&a_t;</trd>
  </trdgrp>
  <trd lng="fr">amiti&eacute;<klr> (situation)</klr></trd>
  <trd lng="hu">bar&aacute;ts&aacute;g</trd>
  <trd lng="id">[<ind>sahabat</ind>] persahabatan</trd>
  <trd lng="it">amicizia</trd>
  <trd lng="nl">vriendschap</trd>
  <trd lng="no">vennskap</trd>
  <trd lng="pl">przyja&zacute;&nacute;</trd>
  <trd lng="ru">&c_d;&c_r;&c_u;&c_zh;&c_b;&c_a;</trd>
  <trd lng="sk">priate&lcaron;stvo</trd>
</drv>

<drv mrk="amik.0igxema">
  <kap><tld/>i&gcirc;ema</kap>
  <snc mrk="amik.0igxema.PSI">
    <dif>
      Kiu facile inter<tld/>i&gcirc;as kun aliaj:
      <ekz>
        virinoj ne estas <tld/>i&gcirc;emaj unu kun alia
        <fnt>
          <aut>Lao She, trad. Wang Chongfang</aut>,
          <vrk><url
          ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0026/">
          Kamelo &Scirc;jangzi</url></vrk>,
          <lok>1988</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en"><trd>sociable</trd>, <trd>friendly</trd></trdgrp>
  <trdgrp lng="es">
    <trd>sociable</trd>,
    <trd>extrovertido</trd>,
    <trd>amistoso</trd>
  </trdgrp>
  <trd lng="fr">liant<klr> (se liant facilement
  d'amiti&eacute;)</klr></trd>
</drv>

<drv mrk="amik.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="amik.0igxi.KOMUNE">
    <dif>
      Starigi <tld/>an rilaton:
      <ekz>
        li <tld/>i&gcirc;is kun malbonaj kolegoj
        <fnt><bib>FK</bib>, <lok>la forgesita pipo</lok></fnt>.
      </ekz>
    </dif>
  </snc>        
  <trd lng="br">mignoni&ntilde;</trd>
  <trd lng="ca">fer <ind>amistat</ind><klr> (amb alg&uacute;)</klr>
  </trd>
  <trd lng="en">to make <ind>friends</ind></trd>
  <trdgrp lng="es">
    <trd>hacer <ind>amistad</ind><klr> (con alguien)</klr></trd>,
    <trd>trabar <ind>amistad</ind><klr> (con alguien)</klr></trd>,
    <trd>hacerse <ind>amigo</ind><klr> (de alguien)</klr></trd>,
    <trd>hacerse amigos</trd>
  </trdgrp>
  <trd lng="fa">&a_d;&a_w;&a_s;&a_t; &a_sh;&a_d;&a_n;</trd>
  <trd lng="fr">se lier d'<ind>amiti&eacute;</ind> avec</trd>
  <trd lng="hu">&ouml;sszebar&aacute;tkozik</trd> 
  <trd lng="id">[<ind>teman</ind>] berteman</trd>
  <trdgrp lng="it">
    <trd>affezionarsi</trd>,
    <trd>diventare <ind>amico</ind><klr> (affezionarsi)</klr></trd>
  </trdgrp> 
  <trd lng="nl">bevriend worden</trd>
  <trdgrp lng="pl">
    <trd>przyja&zacute;ni&cacute; si&eogonek;</trd>,
    <trd>zaprzyja&zacute;ni&cacute; si&eogonek;</trd>
  </trdgrp>
  <trd
  lng="ru">&c_p;&c_o;&c_d;&c_r;&c_u;&c_zh;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="amik.0ino">
  <kap><tld/>ino</kap>
  <snc mrk="amik.0ino.KOMUNE">
    <dif>
      Ino kiu estas en <tld/>aj rilatoj kun aliaj homoj:
      <ekz>
        Sur la kampo for de l' mondo, ||
        anta&ubreve; nokto de somero, ||
        <tld/>ino en la rondo ||
        kantas kanton pri l' espero
        <fnt><aut>&Z;</aut>, <vrk>mia penso</vrk></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="br">
    <trd>keneilez</trd>,
    <trd>mignonez</trd>
  </trdgrp>
  <trd lng="ca">amiga</trd>
  <trd lng="cs">p&rcaron;&iacute;telkyn&ecaron;</trd>
  <trdgrp lng="de">
    <trd>Freundin</trd>,
    <trd>Kameradin</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>female friend</trd>, <trd>girl friend</trd>,
    <trd>girlfriend</trd>
  </trdgrp>
  <trd lng="es">amiga</trd>
  <trd lng="fr">amie</trd>
  <trd lng="hu">bar&aacute;tn&odblac;</trd>
  <trd lng="it">amichetta</trd>
  <trd lng="nl">vriendin</trd>
  <trd lng="no">venninne</trd>
  <trd lng="oc">amiga</trd>
  <trd lng="pl">przyjaci&oacute;&lstroke;ka</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_r;&c_u;&c_g;&c_a;</trd>
  <trd lng="sk">priate&lcaron;ka</trd>
</drv>

<drv mrk="amik.mal0o">
  <kap>mal<tld/>o</kap>
  <snc>
    <dif>
      Persono de kiu oni estas malamata:
      <ekz>
        <ind>estinta <tld/>o estas plej dan&gcirc;era mal<tld/>o</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="hu">a volt bar&aacute;t &aacute;d&aacute;zabb 
        ellens&eacute;g
        </trd>
      </ekz>
    </dif>
    <trd lng="de">Feind</trd>
    <trdgrp lng="fa">
      <trd>&a_d;&a_sh;&a_m;&a_n;</trd>,
      <trd>&a_b;&a_d;&a_kh;&a_w;&a_A;&a_h;</trd>,
      <trd>&a_ayn;&a_d;&a_w;</trd>
    </trdgrp>
    <trd lng="hu">ellens&eacute;g</trd>    
    <trdgrp lng="no">
      <trd>uvenn</trd>,
      <trd>fiende</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_r;&c_a;&c_g;</trd>
    <trdgrp lng="sv">
      <trd>ov&auml;n</trd>,
      <trd>fiende</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Kontra&ubreve;ulo, kontra&ubreve;batalanto.
    </dif>
    <trd lng="de">Gegner</trd>
    <trdgrp lng="fa">
      <trd>&a_r;&a_q;&f_y;&a_b;</trd>,
      <trd>&a_m;&a_kh;&a_A;&a_l;&a_f;</trd>
    </trdgrp>
    <trd lng="hu">ellenf&eacute;l</trd>
    <trd lng="sv">fiende</trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_n;&c_i;&c_k;</trd>
  </snc>
  <snc mrk="amik.mal0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Kontra&ubreve;militanto:
      <ekz>
        mal<tld/>o venis en nian landon
        <fnt><bib>F</bib>, <lok>&FE; 33</lok></fnt>;
      </ekz>
      <ekz>
        la mal<tld/>oj decidis sie&gcirc;i la urbon.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Feind<klr> [se tiu volas trafi la malamikon mem]</klr></trd>,
      <trd>Gegner<klr> [se tiu nur agas kontra&ubreve;e pro 
      kontra&ubreve;aj interesoj]</klr></trd>,
      <trd>milit&auml;rischer Gegner</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_d;&a_sh;&a_m;&a_n;</trd>,
      <trd>&a_kh;&a_S;&a_m;</trd>
    </trdgrp>
    <trd lng="hu">ellens&eacute;g</trd>
    <trd lng="no">fiende</trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_r;&c_a;&c_g;</trd>,
 
<trd>&c_n;&c_je;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_je;&c_l;&c_mol;</trd>,
      <trd>&c_n;&c_je;&c_d;&c_r;&c_u;&c_g;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_n;&c_i;&c_k;</trd>
    </trdgrp>
    <trd lng="sv">fiende</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_o;&c_r;&c_a;&c_g;</trd>,
    <trd>&c_n;&c_je;&c_d;&c_r;&c_u;&c_g;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_c;&c_ib;&c_w;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_r;&c_y;&c_ja;&c_c;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">enebour</trd>
  <trd lng="ca">enemic</trd>
  <trd lng="cs">nep&rcaron;&iacute;tel</trd>
  <trd lng="de">Feind</trd>
  <trd lng="en">enemy</trd>
  <trdgrp lng="es">
    <trd>enemigo</trd>,
    <trd>enemiga</trd>
  </trdgrp>
  <trd lng="fr">ennemi</trd>
  <trdgrp lng="hu">
    <trd>ellens&eacute;g</trd>,
