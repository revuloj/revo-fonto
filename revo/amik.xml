  </trdgrp>
  <trdgrp lng="nl">
    <trd>lief<klr tip="ind"> (subst.)</klr></trd>,
    <trd>vriendin</trd>
  </trdgrp>
  <trd lng="no">kj&aelig;reste</trd>
  <trd lng="pl">dziewczyna</trd>
  <trd lng="pt">namorada</trd>
  <trd lng="ru">&c_v;&c_o;&c_z;&c_l;&c_ju;&c_b;&c_l;&c_je;&c_n;&c_n;&c_a;&c_ja;</trd>
