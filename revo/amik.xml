<drv mrk="amik.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="amik.0i.KOMUNE">
    <dif>
      Esti en <tld/>aj rilatoj:
      <ekz>
        &scirc;i kaj lia fian&ccirc;ino ne <tld/>as
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="https://www.monato.net/2003/007837.html">Ne al
          &ccirc;iuj pla&ccirc;os &ccirc;i literatura gemeto</url></vrk>
        </fnt>;
      </ekz><ekz>
        la olda servistino kaj la infano ne vere inter<tld/>as
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="https://www.monato.net/2003/007837.html">Ne al
          &ccirc;iuj pla&ccirc;os &ccirc;i literatura gemeto</url></vrk>
        </fnt>;
      </ekz><ekz>
        kiam la Poeto <tld/>as kun Muzo, &scirc;i kantas per juna,
        pura, bela, gaja, ama vo&ccirc;o
        <fnt>
          <aut>B. Kolker</aut>,
          <vrk>Poeto de amo kaj revolucio</vrk>,
          <bib>Monato</bib>, <lok>1996:9, p. 24a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_ja;&c_b;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_d;&c_r;&c_u;&c_zh;&c_a;</trd>
  <trd lng="cs">p&rcaron;&aacute;telit se</trd>
  <trdgrp lng="de">
    <trd>lieben</trd>,
    <trd>befreundet sein</trd>
  </trdgrp>
  <trd lng="en">to be friends</trd>
  <trdgrp lng="es">
    <trd>ser amigo</trd>,
    <trd>ser amiga</trd>
  </trdgrp>
  <trd lng="fa">&a_d;&a_w;&a_s;&a_t; &a_b;&a_w;&a_d;&a_n;</trd>
  <trdgrp lng="hu">
    <trd>bar&aacute;tkozik</trd>,
    <trd>bar&aacute;ts&aacute;gban van</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>ber<ind>teman</ind></trd>,
    <trd>ber<ind>sahabat</ind></trd>
  </trdgrp>
  <trd lng="it">essere <ind>amico</ind></trd>
  <trd lng="nl">bevriend zijn met</trd>
  <trd lng="pl">przyja&zacute;ni&cacute; si&eogonek;</trd>
  <trdgrp lng="ro">
    <trd>deveni</trd>,
    <trd>amici</trd>
  </trdgrp> 
  <trd lng="ru">&c_d;&c_r;&c_u;&c_zh;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">priateli&tcaron; sa</trd>
</drv>

<drv mrk="amik.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="amik.0ajxo.KOMUNE">
    <dif>
      <tld lit="A"/>a ago, montro de <tld/>eco:
      <ekz>
        fari <tld/>a&jcirc;on al iu;
      </ekz><ekz>
        sinjoro Benojto foriris, tute mirigita de tia neatendita
        <tld/>a&jcirc;o
        <fnt>
          <vrk><url ref="https://eo.wikisource.org/wiki/Kastelo_de_Prelongo/%C4%88apitro_Unua">Kastelo
          de Prelongo</url></vrk>
        </fnt>;
      </ekz><ekz>
        ne faru al ili mal<tld/>a&jcirc;on kaj ne batalu kontra&ubreve; ili
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 2:19</lok>
        </fnt>;
      </ekz><ekz>
	per <tld/>a&jcirc;o oni &ccirc;iam povas atingi pli multe ol per
	maldelikata forto
        <fnt><aut>John Charles O'Connor</aut><vrk>Esperanto: The
        Student's Complete Text Book</vrk>
        <lok>jaro 1903, p. 69
