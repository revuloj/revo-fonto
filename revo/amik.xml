<drv mrk="amik.0ino">
  <kap><tld/>ino</kap>
  <snc mrk="amik.0ino.KOMUNE">
    <dif>
      Inseksa <ref tip="super" cel="amik.0o"><tld/>o</ref>:
      <ekz>
        <tld/>ino en la rondo 
        kantas kanton pri l' espero
        <fnt><aut>&Z;</aut>, <vrk>mia penso</vrk></fnt>;
      </ekz><ekz>
        &ccirc;u vi amba&ubreve; ne estas <tld/>inoj de la infaneco?
        <fnt><bib>Marta</bib></fnt>.
      </ekz><ekz>
        mi lo&gcirc;as <klr>[&#x2026;]</klr> kun <tld/>ino kiu luas &ccirc;ambron
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9401-telefona-labirinto.txt">Will
          Green kondukas vin tra sia</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_ja;&c_b;&c_r;&c_o;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_ja;&c_c;&c_je;&c_l;&c_mol;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_p;&c_r;&c_i;&c_ja;&c_t;&c_je;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_d;&c_r;&c_u;&c_g;&c_a;&c_r;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&#xF42;&#xFB2;&#xF7C;&#xF42;&#xF66;&#xF0B;&#xF58;&#xF7C;&#xF0B;</trd>
