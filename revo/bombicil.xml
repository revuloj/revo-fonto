<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">

<kap>
  <rad>bombicil</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="bombicil.0o">
  <kap><tld/>o</kap>
  <snc mrk="bombicil.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Birdo karakterizata de plumtufo
      sur la kapo; genro el familio bombiciledoj
      (<trd lng="la">Bombycilla</trd>).
    </dif>
  </snc>
  <trd lng="ca">g&egrave;nere Bombycilla</trd>
  <trd lng="de">Seidenschwanz<klr tip="amb"> (Vogel)</klr></trd>
  <trd lng="en">Bohemian Waxwing</trd>
  <trd lng="fr">jaseur bor&eacute;al</trd>
  <trd lng="hu">selyemfark&uacute; mad&aacute;r</trd>
  <trd lng="pl">jemio&lstroke;uszka</trd>
</drv>
</art>

