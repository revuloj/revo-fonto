<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bombicil.xml,v 1.12 2019/01/22 06:10:14 revo Exp $">

<kap>
  <rad>bombicil</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="bombicil.0o">
  <kap><tld/>o</kap>
  <snc mrk="bombicil.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="bird.0o" lst="voko:birdoj">Birdo</ref> 
      karakterizata de plumtufo
      sur la kapo; 
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref>
      (<trd lng="la">Bombycilla</trd>)
      kun tri specioj 
      el la ordo de <ref tip="malprt"
      cel="paser.0oformaj">paseroformaj</ref>
      birdoj:
      <ekz>
        sorpoj, estas grava man&gcirc;o <klr>[&#8230;]</klr> por multaj birdoj, notinde la Ru&gcirc;aksela
        turdo, la Griza turdo, la Merlo, la Viskoturdo kaj la <tld lit="B"/>o, kiuj siavice
        disigas la semojn per siaj feka&jcirc;oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=310328"
              >Birdokaptista sorbuso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/c/c3/Bombycilla_cedrorum_CT2.jpg/640px-Bombycilla_cedrorum_CT2.jpg"
      prm="CC BY-SA 3.0"
      >Cedra <tld/>o (Bombycilla cedrorum), Kebeko, Kanado
        <fnt>
          <aut>Cephas, propra verko</aut>
          <lok><url ref="&WCW;/File:Bombycilla_cedrorum_CT2.jpg"
              >https://commons.wikimedia.org/wiki/File:Bombycilla_cedrorum_CT2.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="ca">g&egrave;nere Bombycilla</trd>
  <trd lng="cs">brkoslav</trd>
  <trd lng="de">Seidenschwanz<klr tip="amb"> (Vogel)</klr></trd>
  <trd lng="en">Bohemian Waxwing</trd>
  <trd lng="fr">jaseur bor&eacute;al</trd>
  <trd lng="hu">selyemfark&uacute; mad&aacute;r</trd>
  <trd lng="pl">jemio&lstroke;uszka</trd>
  <trd lng="sk">chochol&aacute;&ccaron;</trd>
</drv>
</art>

