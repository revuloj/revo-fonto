<vortaro>
<art mrk="$Id$">

<kap>
  <rad>bombicil</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="bombicil.0o">
  <kap><tld/>o</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="bombicil.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro de birdoj el familio bombiciledoj karakterizata de plumtufo
      sur la kapo (<trd lng="la">Bombycilla</trd>).
    </dif>
  </snc>
  <trd lng="de">Seidenschwanz<klr tip="amb"> (Vogel)</klr></trd>
  <trd lng="en">Bohemian Waxwing</trd>
  <trd lng="fr">jaseur bor&eacute;al</trd>
  <trd lng="pl">jemio&lstroke;uszka</trd>
</drv>
</art>

