<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: introvertit.xml,v 1.15 2023/10/17 16:46:14 revo Exp $">
<kap>
  <rad>introvertit</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="introvertit.0o">
  <kap><tld/>o</kap>
  <snc mrk="introvertit.0o.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      <ref tip="dif" cel="introvert.0ulo.PSI">Introvertulo</ref>:
      <ekz>
        ekstravertito kaj <tld/>o
        <fnt>
          <aut>Dmitrio Lytov</aut>
          <vrk><url ref="&Monato;/2003/008175.html">Novaj
          ideoj jungitaj al malnovaj teorioj</url></vrk>
          <bib>Monato</bib>
          <lok>2003/12, p. 14</lok>
        </fnt>;
      </ekz>
      <ekz>
        sentemaj homoj ofte fermas sian animon, fermas sian koron;
        hodia&ubreve; pli multaj homoj estas <tld/>oj
        <fnt>
          <aut>Jacek</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/39838/154298">
          Sentoj</url></vrk>,
          <lok>2009-05-24</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_t;&c_r;&c_a;&c_v;&c_e;&c_r;&c_t;</trd>
  <trd lng="ca">introvertit<klr> (subst.)</klr></trd>
  <trdgrp lng="cs">
    <trd>introvert</trd>,
    <trd>introverzn&iacute; &ccaron;lov&ecaron;k</trd>
  </trdgrp>
  <trd lng="de">Introvertierter</trd>
  <trd lng="en">introvert</trd>
  <trd lng="fr">introverti (subst.)</trd>
  <trd lng="ja">&#x5185;&#x5411;&#x6027;&#x306E;&#x4EBA;  <pr>&#x306A;&#x3044;&#x5411;&#x305B;&#x3044;&#x306E;&#x3058;&#x3093;</pr></trd>
  <trd lng="nl">introverte persoon</trd>
  <trd lng="pt">introvertido</trd>
  <trd lng="sk">introvert</trd>
</drv>

<drv mrk="introvertit.0a">
  <kap><tld/>a</kap>
  <snc mrk="introvertit.0a.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      <ref tip="dif" cel="introvert.0a.PSI">Introverta</ref>:
      <ekz>
        li estis tre <tld/>a persono &mdash; rememoris <nom>Benczik</nom>, &mdash;
        li havis tute propran originalan imagon pri la mondo,
        pri la objektoj
        <fnt>
          <aut>G. Mi&hcirc;alkov</aut>,
          <vrk><url ref="http://www.autodidactproject.org/other/toth3.html">
          Mi ne deziras esti Juvelo</url></vrk>,
          <lok>en: Beletraj Eseoj, 1987</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_t;&c_r;&c_a;&c_v;&c_e;&c_r;&c_t;&c_n;&c_y;</trd>
  <trd lng="ca">introvertit<klr> (adj.)</klr></trd>
  <trd lng="cs">introvertn&iacute;</trd>
  <trdgrp lng="de">
    <trd>introvertiert</trd>,
    <trd>verschlossen</trd>
  </trdgrp>
  <trd lng="en">introverted</trd>
  <trd lng="fr">introverti (adj.)</trd>
  <trd lng="nl">introvert</trd>
  <trdgrp lng="pt">
    <trd>retra&iacute;do</trd>
    <trd>introvertido</trd>
  </trdgrp>
  <trd lng="sk">introvertn&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_ib;&#x43D;&c_t;&c_r;&#x43E;&c_v;&c_je;&c_r;&c_t;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_a;&#x43C;&#x43A;&#x43D;&c_u;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>

