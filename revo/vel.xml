    <trdgrp lng="hu">
      <trd>&iacute;nyvitorla</trd>,
      <trd>l&aacute;gy <ind>&iacute;ny</ind></trd>,
      <trd>velum</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>velo</trd>,
      <trd><ind>palato</ind> molle</trd>,
      <trd><ind>palato</ind> posteriore</trd>
    </trdgrp>
    <trd lng="pl">podniebienie mi&eogonek;kkie</trd>
    <trd lng="ro">palat moale</trd>
    <trd lng="ru">&c_m;&c_ja;&c_g;&c_k;&c_o;&c_je;
    <ind>&c_n;&c_jo;&c_b;&c_o;</ind></trd>
  </snc>
  <trd lng="af">vaar</trd>
  <trd lng="am">&#x1218;&#x122D;&#x12A8;&#x1265;</trd>
