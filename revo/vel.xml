<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vel</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vel.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Granda forta tuko, fiksita &ccirc;e la mastoj kaj stre&ccirc;ita por
      kapti la venton:
      <ekz>
        ferli, malferli, brajli, hisi la <tld/>ojn;
      </ekz>
      <ekz>
        kvadrata, latina <tld/>o.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_je;&c_t;&c_r;&c_a;&c_z;&c_mol;</trd>
    <trd lng="de">Segel</trd>
    <trd lng="es">vela</trd>
    <trdgrp lng="fr">
      <trd>voile<klr> (marine)</klr></trd>
    </trdgrp>
    <trd lng="hu">vitorla</trd>
    <trd lng="it">vela</trd>
    <trd lng="nl">zeil</trd>
    <trd lng="pl">&zdot;agiel</trd>
    <trd lng="pt">vela</trd>
    <trd lng="ru">&c_p;&c_a;&c_r;&c_u;&c_s;</trd>
    <trd lng="sv">segel</trd>
  </snc>
  <snc mrk="vel.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <uzo tip="fak">FON</uzo>
      <ref tip="dif" cel="palat.post0o.ANA">postpalato</ref>
    <trd lng="be">&c_m;&c_ja;&c_k;&c_k;&c_a;&c_je; &c_p;&c_a;&c_d;&c_n;&c_ja;&c_b;&c_je;&c_n;&c_mol;&c_n;&c_je; <klr>(&c_a;&c_n;&c_a;&c_t;.)</klr></trd>
    <trdgrp lng="fr">
      <trd>palais mou</trd>,
      <trd>voile du palais</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&iacute;nyvitorla</trd>,
      <trd>l&aacute;gy <ind>&iacute;ny</ind></trd>,
      <trd>velum</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>velo</trd>,
      <trd><ind>palato</ind> molle</trd>,
      <trd><ind>palato</ind> posteriore</trd>
    </trdgrp>
    <trd lng="pl">j&eogonek;zyczek podniebienny</trd>
    <trd lng="ru">&c_m;&c_ja;&c_g;&c_k;&c_o;&c_je; <ind>&c_n;&c_jo;&c_b;&c_o;</ind></trd>
  </snc>
</drv>

<drv mrk="vel.antaux0o">
  <kap>anta&ubreve;<tld/>o, <var><kap>front<tld/>o</kap></var></kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="vel.0o.MAR"><tld lit="V"/>o</ref>,
      fiksita &ccirc;e la
      <ref tip="vid" cel="mast.front0o">anta&ubreve;masto</ref>:
      <ekz>
        suprenlevinte la anta&ubreve;<tld/>on kontra&ubreve; la vento,
        ili sin direktis al la sabla&jcirc;o
        <fnt><bib>NT</bib><lok>&Ago; 27:40</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="jxib.0o">&jcirc;ibo</ref>,
      <ref cel="vel.staj0o">stajvelo</ref>
    </refgrp>
  </snc>
  <trd lng="ru">&c_f;&c_o;&c_k;</trd>
  <trd lng="en">foresail</trd>
  <trdgrp lng="de">
    <trd>Fock</trd>,
    <trd>Focksegel</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>foc</trd>,
    <trd>misaine</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>orrvitorla</trd>,
    <trd>el&odblac;t&ouml;rzsvitorla</trd>
  </trdgrp>
  <trd lng="it">fiocco<klr> (naut.)</klr></trd>
</drv>

<drv mrk="vel.bram0o">
  <kap>bram<tld/>o</kap>
  <snc mrk="vel.bram0.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      <ref tip="super" cel="vel.0o.MAR"><tld lit="V"/>o</ref>
      de <ref tip="vid" cel="bram.0o.MAR">brammasto</ref>,
      tria, kalkulante de malsupre.
    </dif>
  </snc>
  <trd lng="de">Bramsegel</trd>
  <trd lng="en">top-gallant</trd>
  <trd lng="fr">perroquet</trd>
  <trd lng="hu">sud&aacute;rvitorla</trd>
  <trd lng="nl">bramzeil</trd>
  <trd lng="ru">&c_b;&c_r;&c_a;&c_m;&c_s;&c_je;&c_l;&c_mol;</trd>
</drv>

<drv mrk="vel.bumbram0o">
  <kap>bumbram<tld/>o, <var><kap>re&gcirc;<tld/>o</kap></var></kap>
  <snc mrk="vel.bumbram0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Kvara <ref tip="super" cel="vel.0o.MAR"><tld/>o</ref>,
      kalkulante de malsupre.
    </dif>
  </snc>
  <trd lng="de">Royal</trd>
  <trd lng="en">royal</trd>
  <trd lng="hu">p&oacute;znavitorla</trd>
  <trd lng="ru">&c_b;&c_o;&c_m;-&c_b;&c_r;&c_a;&c_m;&c_s;&c_je;&c_l;&c_mol;</trd>
</drv>

<drv mrk="vel.cxef0o">
  <kap>&ccirc;ef<tld/>o, <var><kap>grand<tld/>o</kap></var></kap>
  <snc>
  <uzo tip="fak">MAR</uzo>
    <dif>
      <ref tip="super" cel="vel.0o.MAR"><tld lit="V"/>o</ref>,
      fiksita &ccirc;e la
      <ref tip="vid" cel="mast.cxef0o">&ccirc;efmasto</ref>.
    </dif>
  </snc>
  <trd lng="ru">&c_g;&c_r;&c_o;&c_t;<klr tip="amb"> (&c_p;&c_a;&c_r;&c_u;&c_s;)</klr></trd>
  <trd lng="en">main sail</trd>
  <trd lng="de">Gro&szlig;segel</trd>
  <trdgrp lng="hu">
    <trd>f&odblac;t&ouml;rzsvitorla</trd>,
    <trd>f&odblac;vitorla</trd>
  </trdgrp>
  <trd lng="it">randa</trd>
</drv>

<drv mrk="vel.flank0o">
  <kap>flank<tld/>o</kap>
  <snc>
  <uzo tip="fak">MAR</uzo>
    <dif>
      Ekstra
      <ref tip="super" cel="vel.0o.MAR"><tld/>o</ref>,
      kun trapeza formo, kiun oni metas flanke de
      <ref tip="vid" cel="vel.rekta0o.MAR">rekta <tld/>o</ref>.
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>sz&aacute;rnyvitorla</trd>,
    <trd>toldatvitorla</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_i;&c_s;&c_je;&c_l;&c_mol;</trd>
  <trd lng="en">studding-sail</trd>
</drv>

<drv mrk="vel.oblikva0o">
  <kap>oblikva <tld/>o</kap>
  <snc mrk="vel.oblikva0o.MAR">
  <uzo tip="fak">MAR</uzo>
    <dif>
      <ref tip="super" cel="vel.0o.MAR"><tld lit="V"/>o</ref>,
      situanta la&ubreve;longe de la &scirc;ipo, ofte triangula.
    </dif>
  </snc>
  <trd lng="de">Gaffelsegel</trd>
  <trd lng="fr">voile go&eacute;lette</trd>
  <trd lng="hu">&eacute;lvitorla</trd>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_o;&c_j; <ind>&c_p;&c_a;&c_r;&c_u;&c_s;</ind></trd>
</drv>

<drv mrk="vel.rekta0o">
  <kap>rekta <tld/>o, <var><kap>kvadrata <tld/>o</kap></var></kap>
  <snc mrk="vel.rekta0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Rektangula <ref tip="super" cel="vel.0o.MAR"><tld/>o</ref>,
      pendanta sur velstango, situanta transverse de la &scirc;ipo.
    </dif>
  </snc>
  <trd lng="de">Quersegel</trd>
  <trd lng="fr">voile carr&eacute;</trd>
  <trd lng="hu">keresztvitorla</trd>
  <trd lng="it"><ind>vela</ind> quadra</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_ja;&c_m;&c_o;&c_j; <ind>&c_p;&c_a;&c_r;&c_u;&c_s;</ind></trd>,
    <trd>&c_p;&c_o;&c_p;&c_je;&c_r;&c_je;&c_ch;&c_n;&c_y;&c_j; <ind>&c_p;&c_a;&c_r;&c_u;&c_s;</ind></trd>,
  </trdgrp>
</drv>

<drv mrk="vel.staj0o">
  <kap>staj<tld/>o</kap>
  <snc mrk="vel.staj0o.MAR">
  <uzo tip="fak">MAR</uzo>
    <dif>
      Triangula <ref tip="super" cel="vel.oblikva0o.MAR">oblikva
      <tld/>o</ref>,
      hisata la&ubreve;
      <ref tip="vid" cel="staj.0o">stajo</ref>
      kaj situanta &ccirc;e la pruo a&ubreve; inter mastoj.
    </dif>
  </snc>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_k;&c_s;&c_je;&c_l;&c_mol;</trd>
  <trd lng="en">staysail</trd>
  <trd lng="de">Flieger</trd>
  <trdgrp lng="hu">
    <trd>tarcsvitorla</trd>,
    <trd>el&odblac;k&ouml;t&eacute;l-vitorla</trd>
  </trdgrp>
  <trd lng="it"><ind>vela</ind> di straglio</trd>
</drv>

<drv mrk="vel.top0o">
  <kap>top<tld/>o</kap>
  <snc mrk="vel.top0.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      <ref tip="super" cel="vel.0o.MAR"><tld lit="V"/>o</ref>
      de <ref tip="vid" cel="mast.top0o">topmasto</ref>,
      dua, kalkulante de malsupre.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Toppsegel</trd>,
    <trd>Marssegel</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>top sail</trd>,
    <trd>tops'l</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>cs&uacute;csvitorla</trd>,
    <trd>der&eacute;kvitorla</trd>
  </trdgrp>
  <trd lng="nl">marszeil</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_r;&c_s;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_t;&c_o;&c_p;&c_s;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="vel.skun0o">
  <kap>skun<tld/>o</kap>
  <snc mrk="vel.skun0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Kvarangula <ref tip="super" cel="vel.oblikva0o.MAR">oblikva
      <tld/>o</ref>,
      fiksita &ccirc;e <ref tip="vid" cel="gaf.0o">gafo</ref>.
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>csonkavitorla</trd>,
    <trd>gaffvitorla</trd>
  </trdgrp>
  <trd lng="it"><ind>vela</ind> aurica</trd>
  <trd lng="ru">&c_g;&c_a;&c_f;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_p;&c_a;&c_r;&c_u;&c_s;</trd>
</drv>
</art>
