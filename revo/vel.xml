<drv mrk="vel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vel.0o.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      Granda forta tuko, fiksita &ccirc;e masto kaj stre&ccirc;ita por
      kapti la venton:
      <ekz>
        ferli, malferli, brajli, hisi la <tld/>ojn;
      </ekz>
      <ekz>
        kvadrata, latina <tld/>o;
      </ekz>
      <ekz>nur unu <tld/>o estis levita, &ccirc;ar estis nenia bloveto
        <fnt><bib>Fab1</bib>, <lok>La virineto de maro</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_je;&c_t;&c_r;&c_a;&c_z;&c_mol;</trd>
    <trd lng="de">Segel</trd>
    <trd lng="en">sail</trd>
    <trd lng="es">vela</trd>
    <trd lng="fr">voile<klr> (marine)</klr></trd>
    <trd lng="hu">vitorla</trd>
    <trd lng="it">vela</trd>
    <trd lng="nl">zeil</trd>
    <trd lng="pl">&zdot;agiel</trd>
    <trd lng="pt">vela</trd>
    <trd lng="ro">vel&abreve;</trd>
    <trd lng="ru">&c_p;&c_a;&c_r;&c_u;&c_s;</trd>
    <trd lng="sv">segel</trd>
  </snc>
  <snc mrk="vel.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <uzo tip="fak">FON</uzo>
      <ref tip="dif" cel="palat.post0o.ANA">postpalato</ref>
    <trd lng="be">&c_m;&c_ja;&c_k;&c_k;&c_a;&c_je;
    &c_p;&c_a;&c_d;&c_n;&c_ja;&c_b;&c_je;&c_n;&c_mol;&c_n;&c_je;
    <klr>(&c_a;&c_n;&c_a;&c_t;.)</klr></trd>
