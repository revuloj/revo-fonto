<drv mrk="vel.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="vel.0i.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      Veturi per <tld/>oj, pu&scirc;ata de vento:
      <ekz>
        du marojn inter kiuj staras Korint', mi <tld/>is
        <fnt>
          <aut>J. Racine, trad. G. Lagrange</aut>,
          <vrk>Fedra</vrk>,
          <lok>1991</lok>
        </fnt>;
      </ekz>
      <ekz>
        la boato priskribis kurban linion kaj tuj poste &gcirc;i <tld/>is tra du gvidmarkiloj
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        oni ludis pie kaj intense <klr>[&#8230;]</klr> biletoj de kvindek kaj cent kronoj gaje
        <tld/>is tien kaj reen sur la tablo
        <fnt>
          <bib>HsT</bib>
          <lok>Sia propra mastro</lok>
        </fnt>;
      </ekz>
      <ekz>
        Komence anseraro, blankaj cignoj <tld/>as, &#11005; Dorse vent&#8217;, kiel falko, ilin kune
        pelas
        <fnt>
          <bib>SrT</bib>
          <lok>Libro III.</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>plachtit</trd>,
    <trd>plout pod plachtami</trd>
  </trdgrp>
  <trd lng="de">segeln</trd>
  <trd lng="en">sail</trd>
  <trd lng="fr">faire <ind>voile</ind></trd>
  <trd lng="ja">&#24070;&#36208;&#12377;&#12427;  <pr>&#12399;&#12435;&#12381;&#12358;&#12377;&#12427;</pr></trd>
  <trd lng="ne">&#2346;&#2366;&#2354;</trd>
  <trd lng="pl">&zdot;eglowa&cacute;</trd>
  <trd lng="ro">naviga</trd>
  <trd lng="sk">plachti&tcaron; <klr>(plachetnicou)</klr></trd>
  <trd lng="uk">&c_ib;&c_t;&c_i; &c_p;&c_ib;&c_d; &c_v;&c_ib;&c_t;&c_r;&c_i;&c_l;&c_o;&c_m;/&c_p;&c_a;&c_r;&c_u;&c_s;&c_o;&c_m;</trd>
</drv>

<drv mrk="vel.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="vel.0aro.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      Tuto da <tld/>oj de &scirc;ipo:
      <ekz>
        veturi kun plena <tld/>aro
        <fnt><bib>PIV2</bib>, <lok>Artikolo
        &leftquot;vel&rightquot;</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_v;&c_s;&c_i;&c_ch;&c_k;&c_i; &c_p;&c_l;&c_a;&c_t;&c_n;&c_a; &c_n;&c_a;
    &c_je;&c_d;&c_i;&c_n; &c_k;&c_o;&c_r;&c_a;&c_b;</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_a;&c_r;&c_u;&c_s;&c_n;&c_o;&c_je;
      &c_v;&c_o;&c_o;&c_r;&c_u;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_p;&c_a;&c_r;&c_u;&c_s;&c_a;<klr> (&c_v;&c_s;&c_je;
      &c_n;&c_a; &c_k;&c_o;&c_r;&c_a;&c_b;&c_l;&c_je;)</klr></trd>,
      <trd>&c_p;&c_a;&c_r;&c_u;&c_s;&c_n;&c_o;&c_s;&c_t;&c_mol;<klr>
      (&c_k;&c_o;&c_r;&c_a;&c_b;&c_l;&c_ja;)</klr></trd>
    </trdgrp>
  </snc>
  <ref tip="hom" cel="velar.0o">velaro</ref>
  <trd lng="cs">plachtov&iacute;</trd>
  <trd lng="de">alle <ind>Segel</ind></trd>
  <trd lng="en">sails</trd>
  <trd lng="pl">o&zdot;aglowanie</trd>
  <trd lng="ro">greement</trd>
  <trd lng="sk">plachtovie</trd>
</drv>

<drv mrk="vel.V0aro">
  <kap><tld lit="V"/>aro
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
  </kap>
  <mlg>Vel</mlg>
  <snc mrk="vel.V0aro.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Sud-hemisfera <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref>
      (<trd lng="la">Vela</trd>):
      <ekz>
        NGC 2547 estas malfermita stelamaso en konstelacio <tld lit="V"/>aro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=112445"
              >NGC 2547</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_V;&c_je;&c_t;&c_r;&c_a;&c_z;&c_ib; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#8217;&c_je;)</klr></trd>
  <trd lng="bg">&c_K;&c_o;&c_r;&c_a;&c_b;&c_n;&c_i; &c_p;&c_l;&c_a;&c_t;&c_n;&c_a;
    (&c_s;&c_malmol;&c_z;&c_v;&c_je;&c_z;&c_d;&c_i;&c_je;)</trd>
  <trd lng="de">Segel des Schiffs <klr>(Sternbild)</klr></trd>
  <trd lng="en">Vela</trd>
  <trd lng="fr">Voiles<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="ja">&#32207;&#24070;  <pr>&#12381;&#12358;&#12411;</pr></trd>
  <trd lng="pl">&Zdot;agiel  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Velele  <klr>(constela&#539;ie)</klr></trd>
  <trd lng="ru">&c_P;&c_a;&c_r;&c_u;&c_s;&c_a;<klr>   (&c_s;&c_o;&c_z;&c_v;&c_je;&c_z;&c_d;&c_i;&c_je;)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_t;&c_r;&c_i;&c_l;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_u;&c_s;&c_i;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_u;&c_s;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_u;&c_k;&c_u;&c_p;&c_n;&c_ib;&c_s;&c_t;&c_mol; &c_v;&c_ib;&c_t;&c_r;&c_i;&c_l;/&c_p;&c_a;&c_r;&c_u;&c_s;&c_ib;&c_v;</trd>
  </trdgrp>
</drv>

<drv mrk="vel.antaux0o">
  <kap>anta&ubreve;<tld/>o, <var><kap>front<tld/>o</kap></var></kap>
  <snc mrk="vel.antaux0o.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      <ref tip="super" cel="vel.0o.SHI"><tld lit="V"/>o</ref>,
      fiksita &ccirc;e la
      <ref tip="vid" cel="mast.front0o">anta&ubreve;masto</ref>:
      <ekz>
        suprenlevinte la anta&ubreve;<tld/>on kontra&ubreve; la vento,
        ili sin direktis al la sabla&jcirc;o
        <fnt><bib>NT</bib><lok>&Ago; 27:40</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="jxib.0o">&jcirc;ibo</ref>,
      <ref cel="vel.staj0o">stajvelo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_f;&c_o;&c_k;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_t;&c_n;&c_o; &c_n;&c_a; &c_p;&c_r;&c_je;&c_d;&c_n;&c_a;&c_t;&c_a; &c_m;&c_a;&c_ch;&c_t;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Fock</trd>,
    <trd>Focksegel</trd>
  </trdgrp>
  <trd lng="en">foresail</trd>
  <trdgrp lng="fr">
    <trd>foc</trd>,
    <trd>misaine</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>orrvitorla</trd>,
    <trd>el&odblac;t&ouml;rzsvitorla</trd>
  </trdgrp>
  <trd lng="it">fiocco<klr> (naut.)</klr></trd>
  <trdgrp lng="pl">
    <trd>fok</trd>,
    <trd>fok&zdot;agiel</trd>
  </trdgrp>
  <trd lng="ro">foc  <klr>(vel&abreve;)</klr></trd>
  <trd lng="ru">&c_f;&c_o;&c_k;</trd>
  <trd lng="uk">&c_k;&c_l;&c_ib;&c_v;&c_je;&c_r;</trd>
</drv>

