<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vel</rad>/o
</kap>
<drv mrk="vel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vel.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Granda forta tuko, fiksita &ccirc;e la mastoj kaj stre&ccirc;ita por
      kapti la venton:
      <ekz>
        ferli, malferli, brajli, hisi la <tld/>ojn;
      </ekz>
      <ekz>
        kvadrata, latina <tld/>o.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_je;&c_t;&c_r;&c_a;&c_z;&c_mol;</trd>
    <trd lng="de">Segel</trd>
    <trd lng="es">vela</trd>
    <trdgrp lng="fr">
      <trd>voile<klr> (marine)</klr></trd>
    </trdgrp>
    <trd lng="hu">vitorla</trd>
    <trd lng="nl">zeil</trd>
    <trd lng="pl">&zdot;agiel</trd>
    <trd lng="pt">vela</trd>
    <trd lng="ru">&c_p;&c_a;&c_r;&c_u;&c_s;</trd>
    <trd lng="sv">segel</trd>
  </snc>
  <snc mrk="vel.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <uzo tip="fak">FON</uzo>
    <dif>
      <ref tip="dif" cel="palat.post0o">Postpalato</ref>.
    </dif>
    <trd lng="be">&c_m;&c_ja;&c_k;&c_k;&c_a;&c_je;
    &c_p;&c_a;&c_d;&c_n;&c_ja;&c_b;&c_je;&c_n;&c_mol;&c_n;&c_je;
    <klr>(&c_a;&c_n;&c_a;&c_t;.)</klr></trd>
    <trdgrp lng="fr">
      <trd>palais mou</trd>,
      <trd>voile du palais</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&iacute;nyvitorla</trd>,
      <trd>l&aacute;gy <ind>&iacute;ny</ind></trd>,
      <trd>velum</trd>
    </trdgrp>
    <trd lng="pl">j&eogonek;zyczek podniebienny</trd>
    <trd lng="ru">&c_m;&c_ja;&c_g;&c_k;&c_o;&c_je;
    <ind>&c_n;&c_jo;&c_b;&c_o;</ind></trd>
  </snc>
</drv>

<drv mrk="vel.antaux0o">
  <kap>anta&ubreve;<tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      <tld lit="V"/>o, situanta anta&ubreve; la frontmasto:
      <ekz>
        suprenlevinte la anta&ubreve;<tld/>on kontra&ubreve; la vento,
        ili sin direktis al la sabla&jcirc;o
        <fnt><bib>NT</bib><lok>&Ago; 27:40</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="jxib.0o">&jcirc;ibo</ref>,
      <ref cel="front.0o">frontvelo</ref>
    </refgrp>
    <ref tip="super" cel="vel.staj0o">stajvelo</ref>
  </snc>
  <trdgrp lng="en">
    <trd>foresail</trd>,
    <trd>jib</trd>,
    <trd>staysail</trd>
  </trdgrp>
  <trd lng="fr">foc</trd>
  <trdgrp lng="de">
    <trd>Fock</trd>,
    <trd>Flieger</trd>,
    <trd>Kl&uuml;ver</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_l;&c_i;&c_v;&c_je;&c_r;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_k;&c_s;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_f;&c_o;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="vel.staj0o">
  <kap>staj<tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Oblikva triangula <tld/>o, pendanta sur
      <ref tip="vid" cel="staj.0o">stajo</ref>
      kaj situanta &ccirc;e la pruo
      (<ref tip="sub" cel="vel.antaux0o">anta&ubreve;velo</ref>)
      a&ubreve; inter mastoj.
    </dif>
  </snc>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_k;&c_s;&c_je;&c_l;&c_mol;</trd>
  <trd lng="en">staysail</trd>
  <trd lng="de">Flieger</trd>
</drv>

<drv mrk="vel.top0o">
  <kap>top<tld/>o</kap>
  <snc mrk="vel.top0.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      <ref tip="super" cel="vel.0o"><tld lit="V"/>o</ref>
      de <ref tip="vid" cel="mast.top0o">topmasto</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Toppsegel</trd>,
    <trd>Marssegel</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>top sail</trd>,
    <trd>tops'l</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>cs&uacute;csvitorla</trd>,
    <trd>der&eacute;kvitorla</trd>
  </trdgrp>
  <trd lng="nl">marszeil</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_r;&c_s;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_t;&c_o;&c_p;&c_s;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
</drv>

</art>
