<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vel</rad>/o
</kap>
<drv mrk="vel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vel.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Granda forta tuko, fiksita &ccirc;e la mastoj kaj stre&ccirc;ita por kapti la
      venton:
      <ekz>
        ferli, malferli, brajli, hisi la <tld/>ojn;
      </ekz>
      <ekz>
        kvadrata, latina <tld/>o.
      </ekz>
    </dif>
    <trd lng="de">Segel</trd>
    <trd lng="es">vela</trd>
    <trdgrp lng="fr">
      <trd>voile<klr> (marine)</klr></trd>
    </trdgrp>
    <trd lng="nl">zeil</trd>
    <trd lng="ru">&c_p;&c_a;&c_r;&c_u;&c_s;</trd>
    <trd lng="sv">segel</trd>
  </snc>
  <snc mrk="vel.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <uzo tip="fak">FON</uzo>
    <dif>
      <ref tip="dif" cel="palat.post0o">Postpalato</ref>.
    </dif>
    <trd lng="ru">&c_m;&c_ja;&c_g;&c_k;&c_o;&c_je; <ind>&c_n;&c_jo;&c_b;&c_o;</ind></trd>
  </snc>
</drv>

<drv mrk="vel.0sxipo">
  <kap><tld/>&scirc;ipo</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      &Scirc;ipo movata de vento, per la <tld/>oj.
    </dif>
  </snc>
  <trd lng="de">Segelschiff</trd>
  <trdgrp lng="es">
    <trd>velero</trd>,
    <trd>barco de vela</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bateau &agrave; <ind>voile</ind></trd>,
    <trd>voilier</trd>
  </trdgrp>
  <trd lng="nl">zeilschip</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_u;&c_s;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
