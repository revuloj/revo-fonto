<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>refer</rad>/i
</kap>
<drv mrk="refer.0i">
  <kap><tld/>i</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="raport.0i.dokumente">Raporti</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>referieren</trd>,
    <trd>vortragen</trd>
  </trdgrp>
  <trd lng="en">scholarly report</trd>
  <trd lng="nl">verslag uitbrengen</trd>
  <trd lng="ru">&c_r;&c_je;&c_f;&c_je;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="refer.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="raport.0o.dokumenta">Raporto</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Referat</trd>,
    <trd>Bericht</trd>,
    <trd>Vortrag</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verslag</trd>,
    <trd>rapport</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_k;&c_l;&c_a;&c_d;</trd>,
    <trd>&c_r;&c_je;&c_f;&c_je;&c_r;&c_a;&c_t;</trd>
  </trdgrp>
</drv>
</art>
