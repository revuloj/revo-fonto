    <dif>
      <ref tip="dif" cel="narkot.0ajxo">Narkota&jcirc;o</ref>:
      <ekz>
        la <tld/>otrafikantoj esperis, ke la a&ubreve;to de Marta senprobleme atingos
        Budape&scirc;ton kaj poste tie la a&ubreve;to subite malaperos
        <fnt>
          <aut>Julian Modest</aut>
            <vrk><url ref="&Monato;/2000/007050.php">La kaptilo</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 21</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_r;&c_k;&c_o;&c_t;&c_y;&c_k;</trd>
  <trd lng="ca">narc&ograve;tic<klr> (subst.)</klr></trd>
  <trdgrp lng="cs">
    <trd>narkotikum</trd>,
    <trd>omamn&aacute; l&aacute;tka</trd>,
    <trd>omamn&yacute; prost&rcaron;edek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Narkotikum</trd>,
    <trd>Bet&auml;ubungsmittel</trd>,
    <trd>Schlafmittel</trd>
  </trdgrp>
  <trd lng="el">&ny;&alfa;&rho;&kappa;&omega;&tau;&jota;&kappa;&omikron_ton;</trd>
  <trd lng="en">narcotic</trd>
  <trd lng="fr">narcotique<klr> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>&eacute;rz&eacute;stelen&iacute;t&odblac;</trd>,
    <trd>narkotikum</trd>,
    <trd>k&aacute;b&iacute;t&oacute;szer</trd>
  </trdgrp>
  <trd lng="ja">&#x9EBB;&#x9154;&#x85AC;  <pr>&#x307E;&#x3059;&#x3044;&#x3084;&#x304F;</pr></trd>
  <trd lng="nl">verdovend middel</trd>
  <trd lng="pl">narkotyk</trd>
  <trd lng="pt">narc&oacute;tico</trd>
  <trdgrp lng="ro">
    <trd>narcotic</trd>,
    <trd>drog</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_r;&c_k;&c_o;&c_t;&c_i;&c_k;</trd>,
    <trd>&c_n;&c_a;&c_r;&c_k;&c_o;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; &c_s;&c_r;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">narkotikum</trd>
  <trdgrp lng="zh">
    <trd>&#x9EBB;&#x9189;&#x836F;&#x54C1; <pr>m&#xE1;zu&#xEC;y&#xE0;op&#x1D0;n</pr></trd>,
    <trd>&#x9EBB;&#x9189;&#x85E5;&#x54C1; <pr>m&#xE1;zu&#xEC;y&#xE0;op&#x1D0;n</pr></trd>,
    <trd>&#x72AF;&#x6BD2; <pr>f&#xE0;nd&#xFA;</pr></trd>
