<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: larm.xml,v 1.38 2025/12/02 06:50:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>larm</rad>/o
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="larm.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="gut.0o.fluero">Guto</ref> de akveca flua&jcirc;o,
      kiu elfluas el la okuloj:
      <ekz>
        viza&gcirc;o <klr>[&#8230;]</klr> surver&scirc;ita de <tld/>oj
        <fnt>
          <bib>IKr</bib>
          <lok>Quo Vadis?</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj de &gcirc;ojo
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj staris &ccirc;e li en la okuloj kaj sur la vangoj
        <fnt>
          <bib>Fab3</bib>
          <lok>Plej bela rozo de la mondo</lok>
        </fnt>;
      </ekz>
      <ekz>
        Vi man&gcirc;igis al ili panon <tld/>an
        <klr>(malsekigitan de <tld/>oj)</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 80:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e malsekaj okuloj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> ploris per maldol&ccirc;aj <tld/>oj
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la gutoj de la roso ili estas miaj <tld/>oj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj &scirc;uldon ne pagas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_l;&c_ja;&c_z;&c_a;</trd>
  <trd lng="bg">&c_s;&c_malmol;&c_l;&c_z;&c_a;</trd>
  <trd lng="bo">&t_ra;&t_la;&t_tsheg;</trd>
  <trd lng="br">daerenn</trd>
  <trd lng="ca">ll&agrave;grima</trd>
  <trd lng="cs">slza</trd>
  <trd lng="de">Tr&auml;ne</trd>
  <trd lng="el">&delta;&alfa_ton;&kappa;&rho;&ypsilon;</trd>
  <trd lng="en">tear</trd>
  <trd lng="es">l&aacute;grima</trd>
  <trd lng="fr">larme</trd>
  <trd lng="hu">k&ouml;nny</trd>
  <trd lng="id">air mata</trd>
  <trdgrp lng="ja">
    <trd>&#x6D99;  <pr>&#x306A;&#x307F;&#12384;</pr></trd>,
    <trd>&#x4E00;&#x6EF4;  <pr>&#12356;&#12387;&#12390;&#x304D;</pr></trd>,
    <trd>&#12375;&#x305A;&#x304F;</trd>
  </trdgrp>
  <trd lng="nl">traan</trd>
  <trd lng="pl">&lstroke;za</trd>
  <trd lng="pt">l&aacute;grima</trd>
  <trd lng="ro">lacrim&abreve; </trd>
  <trd lng="ru">&c_s;&c_l;&c_je;&c_z;&c_a;</trd>
  <trd lng="sk">slza</trd>
  <trd lng="sv">t&aring;r</trd>
  <trd lng="tr">g&ouml;zya&scedil;&inodot;</trd>
  <trd lng="uk">&c_s;&c_l;&c_mol;&c_o;&c_z;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x6CEA; <pr>l&egrave;i</pr></trd>,
    <trd>&#x6DDA; <pr>l&egrave;i</pr></trd>,
    <trd>&#x6CEA;&#x73E0; <pr>l&egrave;izh&umacron;</pr></trd>,
    <trd>&#x6DDA;&#x73E0; <pr>l&egrave;izh&umacron;</pr></trd>,
    <trd>&#x6CEA;&#x6C34; <pr>l&egrave;ishu&#x1D0;</pr></trd>,
    <trd>&#x6DDA;&#x6C34; <pr>l&egrave;ishu&#x1D0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="larm.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Ellasi, ver&scirc;i <tld/>ojn:
      <ekz>
        <tld/>i sen&scirc;eligante cepojn, pro fumo;
      </ekz>
      <ekz>
        rigardi el <tld/>antaj okuloj
        <fnt>
          <bib>StU</bib>
          <lok>Al Romo</lok>
        </fnt>;
      </ekz>
      <ekz>
        Jesuo <tld/>is
        <fnt>
          <bib>NT</bib>
          <lok>Johano 11:35</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ploris kaj <tld/>is, ne povante diri vorton
        <fnt>
          <bib>IKr</bib>
          <lok>En Kvieta Vespero</lok>
        </fnt>;
      </ekz>
      <ekz>
        via koro ne <tld/>as, kiam &gcirc;i ne sentas ploremon, &gcirc;i ne jubilas, kiam &gcirc;i ne
        sentas &gcirc;ojon
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 44</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="plor.0i">plori</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ib;&c_c;&c_mol; <ind>&c_s;&c_mol;&c_l;&c_jo;&c_z;&c_y;</ind></trd>,
    <trd>&c_p;&c_l;&c_a;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_l;&c_a;&c_ch;&c_a;</trd>
  <trdgrp lng="br">
     <trd>daeraoui&ntilde;</trd>,
     <trd>gouela&ntilde;</trd>
  </trdgrp>
  <trd lng="ca">llagrimejar</trd>
  <trdgrp lng="cs">
    <trd>bre&ccaron;et</trd>,
    <trd>prol&eacute;vat slzy</trd>,
    <trd>slzet</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>weinen</trd>,
     <trd>tr&auml;nen</trd>
  </trdgrp>
  <trd lng="el">&kappa;&lambda;&alfa;&jota_ton;&omega;</trd>
  <trdgrp lng="en">
    <trd>cry</trd>,
    <trd>weep</trd>
  </trdgrp>
  <trd lng="es">llorar</trd>
  <trd lng="fr">pleurer</trd>
  <trd lng="hu">k&ouml;nnyezik</trd>
  <trdgrp lng="id">
    <trd><baz>tangis</baz>menangis</trd>,
    <trd><baz>keluar</baz>mengeluarkan air mata</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6D99;&#12434;&#x6D41;&#12377;  <pr>&#x306A;&#x307F;&#12384;&#12434;&#x306A;&#x304C;&#12377;</pr></trd>,
    <trd>&#x6D99;&#12434;&#x6D6E;&#x304B;&#12409;&#x308B;  <pr>&#x306A;&#x307F;&#12384;&#12434;&#12358;&#x304B;&#12409;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>huilen</trd>,
    <trd>tranen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&lstroke;zawi&cacute;</trd>,
    <trd>p&lstroke;aka&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>chorar</trd>,
    <trd>lacrimejar</trd>
  </trdgrp>
  <trd lng="ro">pl&acirc;nge</trd>
  <trd lng="ru">&c_l;&c_i;&c_t;&c_mol; &c_s;&c_l;&c_jo;&c_z;&c_y;</trd>
  <trd lng="sk">slzi&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_l;&c_a;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_l;&c_i;&c_v;&c_a;&c_t;&c_i; &c_s;&c_l;&c_mol;&c_o;&c_z;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x54ED;&#x6CE3; <pr>k&umacron;q&igrave;</pr></trd>,
    <trd>&#x6D41;&#x6CEA; <pr>li&uacute;l&egrave;i</pr></trd>,
    <trd>&#x6D41;&#x6DDA; <pr>li&uacute;l&egrave;i</pr></trd>,
    <trd>&#x843D;&#x6CEA; <pr>lu&ograve;l&egrave;i</pr></trd>,
    <trd>&#x843D;&#x6DDA; <pr>lu&ograve;l&egrave;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="larm.0igi">
  <kap>
    <tld/>igi
  </kap>
  <snc>
    <dif>
      Ka&ubreve;zi elfluadon de <tld/>oj:
      <ekz>
        &scirc;ira vento blovegis <klr>[&#8230;]</klr> <tld/>igante niajn okulojn
        <fnt>
          <bib>StU</bib>
          <lok>Al Romo</lok>
        </fnt>;
      </ekz>
      <ekz>
        ricev<klr>[i&#8230;]</klr> <tld/>igan nova&jcirc;on
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2013/010812.php"
              >Po&scirc;to perdita</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>iga gaso</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2009/009825.php"
              >Nova Jerusalemo</url></vrk>
          <lok>2009</lok>
        </fnt>;
        <trd lng="be">&c_s;&c_mol;&c_l;&c_je;&c_z;&c_a;&c_c;&c_je;&c_ch;&c_n;&c_y; &c_g;&c_a;&c_z;</trd>
        <trd lng="de">Tr&auml;nengas</trd>
        <trd lng="en">teargas</trd>
        <trd lng="fr">gaz lacrymog&egrave;ne</trd>
      </ekz>
      <ekz>
        policistoj <klr>[&#8230;]</klr> &jcirc;etis <tld/>igajn gasbombojn al la protestantoj
        <fnt>
          <bib>Kon11</bib>
          <lok>La 92-a elsendo de Varsovia Vento</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_v;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol; &c_d;&c_a; <ind>&c_s;&c_mol;&c_l;&c_jo;&c_z;&c_a;&c_w;</ind></trd>,
    <trd>&c_z;&c_m;&c_u;&c_sh;&c_a;&c_c;&c_mol; <ind>&c_p;&c_l;&c_a;&c_k;&c_a;&c_c;&c_mol;</ind></trd>
  </trdgrp>
  <trd lng="de"><ind>Tr&auml;ne</ind>n verursachen</trd>
</drv>

</art>
