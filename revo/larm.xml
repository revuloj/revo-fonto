<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: larm.xml,v 1.36 2025/10/20 13:00:26 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>larm</rad>/o
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="larm.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Guto de akveca flua&jcirc;o, kiu elfluas el la okuloj:
      <ekz>
        viza&gcirc;o superver&scirc;ita de <tld/>oj;
      </ekz>
      <ekz>
        <tld/>oj de &gcirc;ojo;
      </ekz>
      <ekz>
        vi man&gcirc;igis al mi panon <tld/>an<fnt>Z</fnt>
        <klr>(malsekigitan de <tld/>oj)</klr>;
      </ekz>
      <ekz>
        <tld/>e malsekaj okuloj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>oj de la roso;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        funebra nigra drapo kun ar&gcirc;entaj <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_l;&c_ja;&c_z;&c_a;</trd>
  <trd lng="bg">&c_s;&c_malmol;&c_l;&c_z;&c_a;</trd>
  <trd lng="bo">&#xF62;&#xF63;&#xF0B;</trd>
  <trd lng="br">daerenn</trd>
  <trd lng="ca">ll&agrave;grima</trd>
  <trd lng="cs">slza</trd>
  <trd lng="de">Tr&auml;ne</trd>
  <trd lng="el">&delta;&alfa_ton;&kappa;&rho;&ypsilon;</trd>
  <trd lng="en">tear</trd>
  <trd lng="es">l&aacute;grima</trd>
  <trd lng="fr">larme</trd>
  <trd lng="hu">k&ouml;nny</trd>
  <trd lng="id">air mata</trd>
  <trdgrp lng="ja">
    <trd>&#x6D99;  <pr>&#x306A;&#x307F;&#12384;</pr></trd>,
    <trd>&#x4E00;&#x6EF4;  <pr>&#12356;&#12387;&#12390;&#x304D;</pr></trd>,
    <trd>&#12375;&#x305A;&#x304F;</trd>
  </trdgrp>
  <trd lng="nl">traan</trd>
  <trd lng="pl">&lstroke;za</trd>
  <trd lng="pt">l&aacute;grima</trd>
  <trd lng="ro">lacrim&abreve; </trd>
  <trd lng="ru">&c_s;&c_l;&c_je;&c_z;&c_a;</trd>
  <trd lng="sk">slza</trd>
  <trd lng="sv">t&aring;r</trd>
  <trd lng="tr">g&ouml;zya&scedil;&inodot;</trd>
  <trd lng="uk">&c_s;&#x43B;&#x44C;&#x43E;&c_z;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x6CEA; <pr>l&#xE8;i</pr></trd>,
    <trd>&#x6DDA; <pr>l&#xE8;i</pr></trd>,
    <trd>&#x6CEA;&#x73E0; <pr>l&#xE8;izh&#x16B;</pr></trd>,
    <trd>&#x6DDA;&#x73E0; <pr>l&#xE8;izh&#x16B;</pr></trd>,
    <trd>&#x6CEA;&#x6C34; <pr>l&#xE8;ishu&#x1D0;</pr></trd>,
    <trd>&#x6DDA;&#x6C34; <pr>l&#xE8;ishu&#x1D0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="larm.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Ellasi, ver&scirc;i <tld/>ojn:
      <ekz>
        <tld/>i sen&scirc;eligante cepojn, pro fumo.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="plor.0i">plori</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ib;&c_c;&c_mol; <ind>&c_s;&c_mol;&c_l;&c_jo;&c_z;&c_y;</ind></trd>,
    <trd>&c_p;&c_l;&c_a;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_l;&c_a;&c_ch;&c_a;</trd>
  <trdgrp lng="br">
     <trd>daeraoui&ntilde;</trd>,
     <trd>gouela&ntilde;</trd>
  </trdgrp>
  <trd lng="ca">llagrimejar</trd>
  <trdgrp lng="cs">
    <trd>bre&ccaron;et</trd>,
    <trd>prol&eacute;vat slzy</trd>,
    <trd>slzet</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>weinen</trd>,
     <trd>tr&auml;nen</trd>
  </trdgrp>
  <trd lng="el">&kappa;&lambda;&alfa;&jota_ton;&omega;</trd>
  <trdgrp lng="en">
    <trd>cry</trd>,
    <trd>weep</trd>
  </trdgrp>
  <trd lng="es">llorar</trd>
  <trd lng="fr">pleurer</trd>
  <trd lng="hu">k&ouml;nnyezik</trd>
  <trdgrp lng="id">
    <trd><baz>tangis</baz>menangis</trd>,
    <trd><baz>keluar</baz>mengeluarkan air mata</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6D99;&#12434;&#x6D41;&#12377;  <pr>&#x306A;&#x307F;&#12384;&#12434;&#x306A;&#x304C;&#12377;</pr></trd>,
    <trd>&#x6D99;&#12434;&#x6D6E;&#x304B;&#12409;&#x308B;  <pr>&#x306A;&#x307F;&#12384;&#12434;&#12358;&#x304B;&#12409;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>huilen</trd>,
    <trd>tranen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&lstroke;zawi&cacute;</trd>,
    <trd>p&lstroke;aka&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>chorar</trd>,
    <trd>lacrimejar</trd>
  </trdgrp>
  <trd lng="ro">pl&acirc;nge</trd>
  <trd lng="ru">&c_l;&c_i;&c_t;&c_mol; &c_s;&c_l;&c_jo;&c_z;&c_y;</trd>
  <trd lng="sk">slzi&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43B;&c_a;&#x43A;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_r;&#x43E;&#x43B;&c_i;&c_v;&c_a;&c_t;&c_i; &c_s;&#x43B;&#x44C;&#x43E;&c_z;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x54ED;&#x6CE3; <pr>k&#x16B;q&#xEC;</pr></trd>,
    <trd>&#x6D41;&#x6CEA; <pr>li&#xFA;l&#xE8;i</pr></trd>,
    <trd>&#x6D41;&#x6DDA; <pr>li&#xFA;l&#xE8;i</pr></trd>,
    <trd>&#x843D;&#x6CEA; <pr>lu&#xF2;l&#xE8;i</pr></trd>,
    <trd>&#x843D;&#x6DDA; <pr>lu&#xF2;l&#xE8;i</pr></trd>
  </trdgrp>
</drv>
</art>
