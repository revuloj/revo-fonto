<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>larm</rad>/o
</kap>
<drv mrk="larm.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Guto de akveca flua&jcirc;o, kiu elfluas el la okuloj:
      <ekz>
        viza&gcirc;o superver&scirc;ita de <tld/>oj;
      </ekz>
      <ekz>
        <tld/>oj de &gcirc;ojo;
      </ekz>
      <ekz>
        vi man&gcirc;igis al mi panon <tld/>an<fnt>Z</fnt>
        <klr>(malsekigitan de <tld/>oj)</klr>;
      </ekz>
      <ekz>
        <tld/>e malsekaj okuloj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>oj de la roso;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        funebra nigra drapo kun ar&gcirc;entaj <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_l;&c_ja;&c_z;&c_a;</trd>
  <trd lng="br">daerenn</trd>
  <trd lng="de">Tr&auml;ne</trd>
  <trd lng="el">&delta;&alfa_ton;&kappa;&rho;&ypsilon;</trd>
  <trd lng="es">l&aacute;grima</trd>
  <trd lng="en">tear</trd>
  <trd lng="fr">larme</trd>
  <trd lng="hu">k&ouml;nny</trd>
  <trd lng="nl">traan</trd>
  <trd lng="pl">&lstroke;za</trd>
  <trd lng="pt">l&aacute;grima</trd>
  <trd lng="ru">&c_s;&c_l;&c_je;&c_z;&c_a;</trd>
  <trd lng="tr">g&ouml;zya&scedil;&inodot;</trd>
  <trd lng="sv">t&aring;r</trd>
</drv>

<drv mrk="larm.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Ellasi, ver&scirc;i <tld/>ojn:
      <ekz>
        <tld/>i sen&scirc;eligante cepojn, pro fumo.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="plor.0i">plori</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ib;&c_c;&c_mol; <ind>&c_s;&c_mol;&c_l;&c_jo;&c_z;&c_y;</ind></trd>,
    <trd>&c_p;&c_l;&c_a;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="br">
     <trd>daeraoui&ntilde;</trd>,
     <trd>gouela&ntilde;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>weinen</trd>,
     <trd>tr&auml;nen</trd>
  </trdgrp>
  <trd lng="el">&kappa;&lambda;&alfa;&jota_ton;&omega;</trd>
  <trdgrp lng="en">
    <trd>cry</trd>,
    <trd>weep</trd>
  </trdgrp>
  <trd lng="fr">pleurer</trd>
  <trd lng="hu">k&ouml;nnyezik</trd>
  <trdgrp lng="nl">
    <trd>huilen</trd>,
    <trd>tranen</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>chorar</trd>,
    <trd>lacrimejar</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_i;&c_t;&c_mol; &c_s;&c_l;&c_jo;&c_z;&c_y;</trd>
</drv>
</art>
