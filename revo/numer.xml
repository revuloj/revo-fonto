<drv mrk="numer.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <mlg>n-ro</mlg>
  <snc>
    <dif>
      Cifero a&ubreve; nombro montranta la vicon de iu objekto inter
      aliaj:
      <ekz>
        <tld/>o de pa&gcirc;o, domo, telefono, bileto.
      </ekz>
    </dif>
  <trdgrp lng="es">
    <trd>n&uacute;mero</trd>,
    <trd>cifra</trd>
  </trdgrp>
  </snc>
  <snc>
    <dif>
      La objekto mem signita per iu <tld/>o:
      <ekz>
        <tld/>o de gazeto;
      </ekz>
      <ekz>
        <ind>gajna <tld/>o</ind> de loterio<fnt>Z</fnt>.
        <trd lng="hu">nyer&odblac;sz&aacute;m</trd>
      </ekz>
    </dif>
  <trd lng="es">ejemplar</trd>
  </snc>
  <trd lng="af">Syfer</trd>
  <trd lng="am">&#x1241;&#x1325;&#x122D;</trd>
  <trd lng="ar">&#x627;&#x644;&#x623;&#x631;&#x642;&#x627;&#x645;</trd>
  <trd lng="az">say</trd>
  <trd lng="be">&c_n;&c_u;&c_m;&c_a;&c_r;</trd>
  <trdgrp lng="bg">
    <trd>&c_n;&c_o;&c_m;&c_je;&c_r;</trd>,
    <trd>&c_b;&c_r;&c_o;&c_j; (&c_n;&c_a; &c_v;&c_i;&c_s;&c_t;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_s;&c_p;&c_i;&c_s;&c_a;&c_n;&c_i;&c_je; &c_i; &c_t;.&c_n;.)</trd>
  </trdgrp>
  <trd lng="bn">&#x9B8;&#x982;&#x996;&#x9CD;&#x9AF;&#x9BE;</trd>
  <trd lng="bo">&#xF42;&#xFB2;&#xF44;&#xF66;&#xF0B;&#xF40;&#xF0B;</trd>
  <trd lng="br">niverenn</trd>
  <trd lng="bs">brojka</trd>
  <trd lng="co">ma&#xEE;tre</trd>
  <trd lng="cs">&ccaron;&iacute;slo</trd>
  <trd lng="da">talord</trd>
