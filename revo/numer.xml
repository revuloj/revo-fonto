<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>numer</rad>/o
</kap>
<drv mrk="numer.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Cifero a&ubreve; nombro montranta la vicon  de iu objekto inter aliaj:
      <ekz>
        <tld/>o de pa&gcirc;o, domo, telefono, bileto.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      La objekto mem signita per iu <tld/>o:
      <ekz>
        <tld/>o de gazeto;
      </ekz>
      <ekz>
        gajna <tld/>o de loterio<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">&ccaron;&iacute;slo</trd>
  <trd lng="de">Nummer</trd>
  <trd lng="en">number</trd>
  <trd lng="es">n&uacute;mero</trd>
  <trd lng="fr">num&eacute;ro</trd>
  <trd lng="nl">nummer</trd>
</drv>
<drv mrk="numer.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Signi ion per <tld/>o:
      <ekz>
        <tld/>i pa&gcirc;ojn de libro, librojn de biblioteko.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">nummerieren</trd>
  <trd lng="es">numerar</trd>
  <trd lng="fr">num&eacute;roter</trd>
  <trd lng="nl">nummeren</trd>
</drv>
<drv mrk="numer.0ebla">
  <kap><tld/>ebla</kap>
  <snc mrk="numer.0ebla.KOMUNE">
    <dif>
      Tia, ke eblas &gcirc;in <tld/>i.
    </dif>
    <trd lng="fr">num&eacute;rotable</trd>
  </snc>
  <snc mrk="numer.0ebla.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <uzo tip="stl">EVI</uzo>
    <ref tip="dif" cel="nombr.0ebla.MAT">nombrebla.</ref>
  </snc>
</drv>
</art>
