  <trd lng="et">numbritega</trd>
  <trd lng="fr">num&eacute;ro</trd>
  <trd lng="gu">&#xA86;&#xA82;&#xA95;&#xAA1;&#xACB;</trd>
  <trd lng="ha">adadi</trd>
  <trd lng="hi">&#x905;&#x902;&#x915;</trd>
  <trd lng="hr">cifra</trd>
  <trd lng="hu">sz&aacute;m</trd>
  <trd lng="hy">&#x569;&#x57E;&#x561;&#x56F;&#x561;&#x576;</trd>
  <trdgrp lng="id">
    <trd>angka</trd>,
    <trd>nomor</trd>
  </trdgrp>
  <trd lng="is">talan</trd>
  <trd lng="ja">&#x6570;&#x5B57;</trd>
  <trd lng="jw">angka</trd>
  <trd lng="ka">&#x10EA;&#x10D8;&#x10E4;&#x10E0;&#x10D4;&#x10D1;&#x10D8;&#x10E1;</trd>
  <trd lng="km">&#x179B;&#x17C1;&#x1781;</trd>
