<drv mrk="numer.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Signi ion per <tld/>o:
      <ekz>
        <tld/>i pa&gcirc;ojn de libro, librojn de biblioteko;
      </ekz>
      <ekz>
        la svati&gcirc;antoj, la&ubreve; la ordo de sia alveno, estis <tld/>ataj kaj starigataj en
        vicoj
        <fnt>
          <bib>Fab3</bib>
          <lok>Johan&ccirc;jo-malsa&gcirc;ulo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Becher</nom> proponis <tld/>i la vortojn de tuta vortaro kaj tiujn &ccirc;i nombrojn uzi
        kiel komunan lingvon de skribado
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        por ilin poluri, mi prenos ilin en mian anta&ubreve;tukon, sed anta&ubreve;e oni devas ilin
        <tld/>i
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la la&ubreve;enhave dividitaj grupoj da proverboj estos <tld/>itaj
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_u;&c_m;&c_a;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_n;&c_o;&c_m;&c_je;&c_r;&c_i;&c_r;&c_a;&c_m;</trd>
  <trd lng="br">niverenni&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>o&ccaron;&iacute;slovat</trd>,
    <trd>&ccaron;&iacute;slovat</trd>
  </trdgrp>
  <trd lng="de">nummerieren</trd>
  <trd lng="es">numerar</trd>
  <trd lng="fr">num&eacute;roter</trd>
  <trd lng="hu">megsz&aacute;moz</trd>
  <trd lng="id">me<ind>nomor</ind>i</trd>
  <trd lng="nl">nummeren</trd>
  <trd lng="pl">numerowa&cacute;</trd>
  <trd lng="pt">numerar</trd>
  <trdgrp lng="ro">
    <trd>num&abreve;ra</trd>,
    <trd>numerota</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_u;&c_m;&c_je;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>o&ccaron;&iacute;slova&tcaron;</trd>,
    <trd>&ccaron;&iacute;slova&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_n;&c_u;&c_m;&c_je;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  <trd lng="zh">&#32534;&#21495; [<ind>b&#299;an h&agrave;o</ind>]</trd>
</drv>

<drv mrk="numer.0ebla">
  <kap><tld/>ebla</kap>
  <snc mrk="numer.0ebla.KOMUNE">
    <dif>
      Tia, ke eblas &gcirc;in <tld/>i.
    </dif>
    <trd lng="fr">num&eacute;rotable</trd>
    <trd lng="hu">megsz&aacute;mozhat&oacute;</trd> 
    <trd lng="pl">przeliczalny</trd> 
    <trd lng="ro">calculat&abreve;</trd>  
  </snc>
  <snc mrk="numer.0ebla.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      <klr>(p.p. <ref cel="ar.0o.MAT">aro</ref>)</klr>
      <ref cel="pov.sam0a.aroj">Sampova</ref>, kiel iu subaro de la aro
      de entjeroj:
      <ekz>
        finia aro estas <tld/>ebla;
      </ekz><ekz>
        la aro de racionaloj estas <tld/>ebla, la aro de reeloj ne
        estas;
      </ekz><ekz>
        <ref cel="spac.banahxa0o">bana&hcirc;a spaco</ref> povas havi
        <tld/>eblan dimension, nur se finidimensia.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="diskre.0a.nombrebla">diskreta</ref>,
      <ref cel="nombr.0ebla.MAT">nombrebla</ref>.
    </refgrp>
    <rim>
      <nom>Werner</nom> uzas por &ccirc;i tiu senco <ctl>nombrebla</ctl> (germane:
      <nac>abz&auml;hlbar</nac>) &ndash; konforma al la nacilingvaj ekvivalentoj &#8208;, kiun li
      distingas disde <ctl><tld/>ebla</ctl> (germane: <nac>numerierbar</nac>,
      malofta en faka kunteksto). Tamen niaj aliaj fontoj mencias nur
      <ctl><tld/>ebla</ctl> kaj ni preferis sekvi ilin. Notindas, ke
      la difino en <fnt><bib>EKV</bib><lok>&para;303</lok></fnt> postulas,
      ke nur nefiniaj aroj povas esti <tld/>eblaj, sed malmultaj
      dividas tiun &ccirc;i vidpunkton. Notu anka&ubreve;, ke <nom>Bricard</nom>
      <fnt><bib>MatTerm</bib><lok>p. 18</lok></fnt> konas terminon
      <ctl>komputebla</ctl>, ver&scirc;ajne samsignifa.
    </rim>    
    <trd lng="br">niveradus</trd>
    <trdgrp lng="de">
      <trd>abz&auml;hlbar</trd>,
      <trd>diskret</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>denumerable</trd>,
      <trd>countable</trd>,
      <trd>discrete</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;nombrable</trd>,
      <trd>discret<klr tip="ind"> (math.)</klr></trd>
    </trdgrp>
    <trd lng="hu">megsz&aacute;ml&aacute;lhat&oacute;</trd> 
    <trdgrp lng="pl">
      <trd>przeliczalny</trd>,
      <trd>dyskretny</trd>
    </trdgrp>  
    <trdgrp lng="ro">
      <trd>calculat&abreve;</trd>,
      <trd>discret</trd>
    </trdgrp> 
    <trdgrp lng="ru">
      <trd>&c_s;&c_ch;&c_jo;&c_t;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_je;&c_ch;&c_i;&c_s;&c_l;&c_i;&c_m;&c_y;&c_j;</trd>,
      <trd>&c_d;&c_i;&c_s;&c_k;&c_r;&c_je;&c_t;&c_n;&c_y;&c_j;</trd>,
    </trdgrp>
    <trdgrp lng="zh">
        <trd>&#21487;&#25968;&#30340; [<ind>k&ecaron; sh&#468; de</ind>]</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ja;&c_k;&c_ib; &c_p;&c_a;&c_d;&c_d;&c_a;&c_je;&c_c;&c_c;&c_a; <ind>&c_p;&c_je;&c_r;&c_a;&c_l;&c_ib;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_ju;</ind></trd>,
    <trd>&c_d;&c_y;&c_s;&c_k;&c_r;&c_e;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="es">numerable</trd>
  <trd lng="uk">&c_ja;&c_k;&c_i;&c_j; &c_p;&c_ib;&c_d;&c_d;&c_a;&c_jeu;&c_t;&c_mol;&c_s;&c_ja; &c_n;&c_u;&c_m;&c_je;&c_r;&c_a;&c_c;&c_ib;&c_ji;</trd>
</drv>

<drv mrk="numer.prov0o">
  <kap>prov<tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="ekzemp1.prov0o">Provekzemplero</ref> de
      gazeto:
      <ekz>
        bonvolu kontakti&gcirc;i kun la komunikadcentro de EEU 
        por legi la prov<tld/>on
        <fnt><lok><url
        ref="http://www.chinareport.com.cn/04e/2003-4.htm">http://www.chinareport.com.cn/04e/2003-4.htm</url></lok></fnt>.
      </ekz>
      <ekz>
        la prov<tld/>o de Lingvo Internacia, decembro 1895
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
          <lok>Enkonduko</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_o;&c_b;&c_n;&c_y; <ind>&c_n;&c_u;&c_m;&c_a;&c_r;</ind> <klr>(&c_p;&c_e;&c_r;&c_y;&c_ja;&c_d;&c_y;&c_ch;&c_n;&c_a;&c_g;&c_a; &c_v;&c_y;&c_d;&c_a;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>
  <trd lng="br">niverenn skouer<klr tip="ind"> 
(kelaouenno&ugrave;)</klr></trd>
  <trd lng="hu">pr&oacute;basz&aacute;m</trd>  
  <trdgrp lng="pl">
    <trd>numer pr&oacute;bny</trd>,
    <trd>numer zerowy</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>num&abreve;rul de &icirc;ncercare</trd>,
    <trd>num&abreve;rul de zero</trd>
  </trdgrp>
</drv>

<drv mrk="numer.signo0o">
  <kap>signo<tld/>o</kap>
  <uzo tip="fak">KOMP</uzo>
  <snc>
    <dif>
      En komputoscienco, nombro per kiu oni kodas
      <ref cel="sign.0o.KOMP">signon<sncref/></ref> en
      <ref cel="kod.0o.SCI">kodo<sncref/></ref>:
      <ekz>
        oni akceptas la literojn <frm><g>&ccirc;, &gcirc;, &hcirc;, &jcirc;, &scirc;, &ubreve;</g></frm> kiel simplajn
        signojn integrajn, kun la signo<tld/>oj 
        <frm>&Ccirc;=264, &Gcirc;=284, &Hcirc;=292,</frm> <klr>&#8230;</klr>
        <fnt><aut>S. Pokrovskij</aut>:
          <vrk><url ref="https://lingvakritiko.com/2018/05/09/grafemo/"
             >Grafemo</url></vrk>.
          <lok>Lingva kritiko, 2018.</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="super" cel="kod.0ero">kodero</ref>
  <trdgrp lng="en">
    <trd>code element</trd>,
    <trd>code point</trd>,
    <trd>code position</trd>
  </trdgrp>
    <trdgrp lng="fr">
    <trd>&eacute;l&eacute;ment de <ind>code</ind></trd>,
    <trd>mot-<ind>code</ind></trd>,
    <trd>codet</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_d;&c_o;&c_v;&c_a;&c_ja; &c_k;&c_o;&c_m;&c_b;&c_i;&c_n;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_k;&c_o;&c_d;&c_o;&c_n;</trd>
  </trdgrp>
</drv>

