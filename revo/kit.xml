<drv mrk="kit.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      La <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&ccirc;efurbo</ref>
      de <ref tip="malprt" cel="ekvador.E0o">Ekvadoro</ref>:
      <ekz>
        la rompi&gcirc;o de diplomatiaj rilatoj inter Karakaso kaj <tld/>o fari&gcirc;is kompleta je
        la 19a de oktobro 2018, post deklaroj de la venezuela ministro
        <fnt>
          <vrk><url ref="&Mondediplo;article2760.html">Le Monde diplomatique' en
          Esperanto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        manifestaciantoj amaseniris en la malplenan parlamentejon en <tld/>o
        <fnt>
          <vrk><url ref="https://eo.wikinews.org/wiki/Manifestaciantoj_devigis_la_ekvadoran_registaron_fu%C4%9Di_el_la_%C4%89efurbo">Manifestaciantoj
          devigis la ekvadoran registaron fu&gcirc;i</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_K;&c_ib;&c_t;&c_a; <klr>(&c_s;&c_t;&c_a;&c_l;&c_ib;&c_c;&c_a; &c_E;&c_k;&c_v;&c_a;&c_d;&c_o;&c_r;&c_a;)</klr></trd>
    <trd lng="de">Quito</trd>
    <trd lng="en">Quito</trd>
    <trd lng="fr">Quito</trd>
  </snc>
  <trd lng="ja">&#x30AD;&#x30C8;</trd>
  <trd lng="uk">&#x41A;&c_ib;&c_t;&#x43E;</trd>
</drv>

