<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tikl</rad>/i
</kap>
<drv mrk="tikl.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ka&ubreve;zi per delikataj kaj ripetaj ektu&scirc;oj de iuj partoj de la korpo
      sentuma&jcirc;on, kiu estigas konvulsian ridon:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu muziko agrable <tld/>as la orelojn.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">kitzeln</trd>
  <trd lng="en">tickle</trd>
  <trd lng="fr">chatouiller</trd>
  <trdgrp lng="nl">
    <trd>kietelen</trd>,
    <trd>kriebelen</trd>
  </trdgrp>
  <trd lng="ru">&c_shch;&c_je;&c_k;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="tikl.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="T"/>anta.
    </dif>
    <trd lng="de">kitzelnd</trd>
    <trd lng="nl">kietelend</trd>
    <trdgrp lng="ru">
      <trd>&c_shch;&c_je;&c_k;&c_o;&c_t;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_shch;&c_je;&c_k;&c_o;&c_ch;&c_u;&c_shch;&c_i;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      <tld lit="T"/>i&gcirc;ema, facile vundebla:
      <ekz>
        tio estas lia <tld/>a (vunda) punkto<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">heikel</trd>
    <trd lng="nl">netelig</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malfacile solvebla:
      <ekz>
        <tld/>a afero<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">heikel</trd>
    <trd lng="nl">netelig</trd>
    <trd lng="ru">&c_shch;&c_je;&c_k;&c_o;&c_t;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
  </snc>
</drv>

<drv mrk="tikl.0igxema">
  <kap><tld/>i&gcirc;ema</kap>
  <snc>
    <dif>
      Tu&scirc;otima, <tld/>osentema, facile impresebla per <tld/>ado.
    </dif>
  </snc>
  <trd lng="de">kitzlig</trd>
  <trd lng="en">ticklish</trd>
  <trd lng="fr">chatouilleux</trd>
  <trdgrp lng="nl">
    <trd>kittelorig</trd>,
    <trd>lichtgeraakt</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_k; <ind>&c_shch;&c_je;&c_k;&c_o;&c_t;&c_k;&c_je;</ind></trd>
</drv>
</art>
