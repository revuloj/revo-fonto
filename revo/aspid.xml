<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aspid.xml,v 1.22 2022/04/25 15:11:00 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>aspid</rad>/o
</kap>
<drv mrk="aspid.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_specioj" cel="speci.0o">Specio</ref>
      de <ref tip="super" cel="vipur.0o">vipuro</ref>
      (<trd lng="la">Vipera aspis</trd>), venena serpenteto:
      <ekz>
        <klr>[li]</klr> trakondukis vin tra la granda kaj terura dezerto, 
        kie estis serpentoj, <tld/>oj kaj skorpioj kaj sekaj lokoj senakvaj
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 8:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        pereiga veneno de <tld/>oj  
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 32:33</lok>
        </fnt>;
      </ekz>
      <ekz>
        venenon de <tld/>oj li su&ccirc;os, lango de vipuro lin mortigos
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 20:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvaza&ubreve; mordita de <tld/>o, Klorinda eksaltis 
        <fnt>
          <bib>Monato</bib>
          <aut>Julia Sigmond kaj Sen Rodin</aut>
          <vrk><url ref="&Monato;/2010/009988.php">La mal&jcirc;aluzo de Klorinda</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/20/Vipera_aspis_Lothringen_123.jpg/640px-Vipera_aspis_Lothringen_123.jpg" prm="CC-BY-SA-3.0"><tld/>ino (Vipera aspis)
        <fnt>
          <aut>Orchi, Self-photographed</aut>
          <lok><url ref="&WCW;/File:Vipera_aspis_Lothringen_123.jpg">&WCW;/File:Vipera_aspis_Lothringen_123.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_mol;&c_p;&c_ib;&c_d; <klr>(&c_z;&c_mol;&c_m;&c_ja;&c_ja;)</klr></trd>
  <trd lng="br">naer-speg</trd>
  <trd lng="ca">escur&ccedil;&oacute; pirinenc</trd>
  <trdgrp lng="cs">
    <trd>brejlovec</trd>,
    <trd>had</trd>,
    <trd>zmije</trd>
  </trdgrp>
  <trd lng="de">Aspisviper</trd>
  <trd lng="en">asp</trd>
  <trd lng="fr">aspic (vip&egrave;re)</trd>
  <trd lng="he">&#x5E6;&#x5E4;&#x5E2;</trd>
  <trdgrp lng="hu">
    <trd>&aacute;spisvipera</trd>,
    <trd>&aacute;spisk&iacute;gy&oacute;</trd>
  </trdgrp>
  <trd lng="it">aspide</trd>
  <trdgrp lng="pt">
    <trd>naja</trd>,
    <trd>&aacute;spide</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_s;&c_p;&c_i;&c_d; <klr>(&c_z;&c_m;&c_je;&c_ja;)</klr></trd>
  <trd lng="sk">druh vretenice</trd>
  <trd lng="uk">&#x430;&#x441;&#x43F;&#x456;&#x434;</trd>
</drv>
</art>
