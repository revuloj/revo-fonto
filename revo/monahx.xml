<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>mona&hcirc;</rad>/o
    </kap>

    <drv mrk="monahx.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <uzo tip="fak">REL</uzo>
      <snc>
    	<dif>
    	  Viro vivanta <tld/>isme la&ubreve; sia religia promeso:
    	  <ekz>
    	    en Athos lo&gcirc;as nun &ccirc;irka&ubreve; 1500 <tld/>oj
	    <fnt>
	      <aut>Evgeni Georgiev</aut>:
	      <vrk>Mona&hcirc;oj defendas &leftquot;liberecon&rightquot;</vrk>,
	      <bib>Monato</bib>
	      <lok>jaro 1997a, numero 8a, p. 13a</lok>
	    </fnt>.
    	  </ekz>
    	</dif>
    	<refgrp tip="sin">
    	  <ref cel="monak.0o">monako</ref>
    	</refgrp>
    	<refgrp tip="sub">
    	  <ref cel="cenobit..0o">cenobito</ref>,
    	  <ref cel="ermit.0o..REL">ermito <sncref/></ref>,
    	  <ref cel="dervisx.0o">dervi&scirc;o</ref>,
    	  <ref cel="lama.0o">lamao</ref>
    	</refgrp>
      </snc>
      <trd lng="be">&c_m;&c_a;&c_n;&c_a;&c_h;</trd>
      <trdgrp lng="de">
    	<trd>M&ouml;nch</trd>
      </trdgrp>
      <trdgrp lng="en">
    	<trd>monk</trd>,
      </trdgrp>
      <trd lng="es">monje</trd>
      <trdgrp lng="fr">
    	<trd>religieux<klr> (moine)</klr></trd>,
    	<trd>moine</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>szerzetes</trd>,
        <trd>bar&aacute;t <klr tip="ind">(szerzetes)</klr></trd>
      </trdgrp>
      <trd lng="nl">monnik</trd>
      <trdgrp lng="ru">
    	<trd>&c_m;&c_o;&c_n;&c_a;&c_h;</trd>,
    	<trd>&c_i;&c_n;&c_o;&c_k;</trd>
      </trdgrp>
    </drv>

    <drv mrk="monahx.0ino">
      <kap><ofc>*</ofc><tld/>ino</kap>
      <uzo tip="fak">REL</uzo>
      <snc>
    	<dif>
    	  Ino vivanta <tld/>isme la&ubreve; sia religia promeso:
    	  <ekz>
    	    albanoj detruis pli ol cent ortodoksajn pre&gcirc;ejojn kaj timigis
    	    <tld/>inojn<fnt><aut>K. Eckstadt</aut>: <vrk>Insultoj, misinformoj
		kaj malveroj</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 3a,
		p. 8a</lok></fnt>.
    	  </ekz>
    	</dif>
      </snc>
      <trd lng="be">&c_m;&c_a;&c_n;&c_a;&c_sh;&c_k;&c_a;</trd>
      <trdgrp lng="de">
    	<trd>Nonne</trd>
      </trdgrp>
      <trdgrp lng="en">
    	<trd>nun</trd>
      </trdgrp>
      <trd lng="es">monja</trd>
      <trdgrp lng="fr">
    	<trd>religieuse<klr> (moniale)</klr></trd>,
    	<trd>moniale</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>ap&aacute;ca</trd>,
        <trd>(szerzetes) <ind>n&odblac;v&eacute;r</ind></trd>
      </trdgrp>
      <trd lng="nl">non</trd>
      <trdgrp lng="ru">
    	<trd>&c_m;&c_o;&c_n;&c_a;&c_h;&c_i;&c_n;&c_ja;</trd>,
        <trd>&c_m;&c_o;&c_n;&c_a;&c_sh;&c_k;&c_a;</trd>,
    	<trd>&c_i;&c_n;&c_o;&c_k;&c_i;&c_n;&c_ja;</trd>
      </trdgrp>
    </drv>

    <drv mrk="monahx.0ejo">
      <kap><ofc>*</ofc><tld/>ejo</kap>
      <uzo tip="fak">REL</uzo>
      <snc>
    	<dif>
    	  Aparta domo, kie kunvivas <tld/>oj:
    	  <ekz>
    	    junulo skribas al iama amikino, kiel li eklo&gcirc;is en
    	    <tld/>ejo<fnt><aut>Edmund Grimley Evans</aut>: <vrk>Denove
		kvaropas (Fernandez, Camacho, Neves, Dek, Ekstremoj)</vrk>,
	      <bib>Monato</bib><lok>jaro 1998a, numero 2a, p. 15a</lok></fnt>.
    	  </ekz>
    	  <ekz>
    	    dum 3 jaroj Alexandra denove studis en <tld/>ejo
    	    <klr>(en &ccirc;i tiu ekzemplo probable ne&ubreve;tre, pri ejo por
	      gemona&hcirc;oj)</klr>
    	    <fnt>
	      <aut>Robert Roze</aut>:
	      <vrk>Verkistino - orientalistino - esploristino</vrk>,
	      <bib>Monato</bib><lok>jaro 1999a, numero 1a, p. 17a</lok></fnt>.
    	  </ekz>
    	</dif>
      </snc>
      <trd lng="be">&c_m;&c_a;&c_n;&c_a;&c_s;&c_t;&c_y;&c_r; <klr tip="amb">(&c_m;&c_u;&c_zh;&c_ch;&c_y;&c_n;&c_s;&c_k;&c_ib;)</klr></trd>
      <trdgrp lng="de">
        <trd>Kloster</trd>
      </trdgrp>
      <trdgrp lng="en">
    	<trd>monastery <klr>(mona&hcirc;ejo)</klr></trd>
      </trdgrp>
      <trd lng="es">monasterio</trd>
      <trdgrp lng="fr">
    	<trd>cloitre<klr> (monast&egrave;re)</klr></trd>,
    	<trd>monast&egrave;re<klr> (masculin)</klr></trd>
      </trdgrp>
      <trdgrp lng="hu">
    	<trd>kolostor</trd>,
    	<trd>rendh&aacute;z</trd>
      </trdgrp>
      <trd lng="nl">klooster</trd>
      <trdgrp lng="ru">
	<trd>&c_m;&c_o;&c_n;&c_a;&c_s;&c_t;&c_y;&c_r;&c_mol;</trd>,
	<trd>&c_m;&c_u;&c_zh;&c_s;&c_k;&c_o;&c_j; &c_m;&c_o;&c_n;&c_a;&c_s;&c_t;&c_y;&c_r;&c_mol;</trd>
      </trdgrp>
    </drv>

    <drv mrk="monahx.0inejo">
      <kap><ofc>*</ofc><tld/>inejo</kap>
      <uzo tip="fak">REL</uzo>
      <snc>
    	<dif>
    	  Aparta domo, kie kunvivas <tld/>inoj:
    	  <ekz>
    	    necesis detrui malnovan <tld/>inejon
	    <fnt>
	      <aut>Sergio Pokrovskij</aut>: <vrk>Moskvo jubileas</vrk>,
	      <bib>Monato</bib><lok>jaro 1997a, numero 9a, p. 22a</lok>
	    </fnt>.
    	  </ekz>
    	</dif>
      </snc>
      <trd lng="be">&c_m;&c_a;&c_n;&c_a;&c_s;&c_t;&c_y;&c_r; <klr tip="amb">(&c_zh;&c_a;&c_n;&c_o;&c_ch;&c_y;)</klr></trd>
      <trdgrp lng="de">
        <trd>Kloster</trd>,
        <trd>Nonnenkloster</trd>
      </trdgrp>
      <trdgrp lng="en">
    	<trd>abbey <klr>(mona&hcirc;inejo)</klr></trd>
      </trdgrp>
      <trd lng="es">convento</trd>
      <trdgrp lng="fr">
    	<trd>cloitre<klr> (couvent)</klr></trd>,
    	<trd>couvent<klr> (f&eacute;minin)</klr></trd>
      </trdgrp>
      <trdgrp lng="hu">
    	<trd>z&aacute;rda</trd>,
    	<trd>ap&aacute;cakolostor</trd>,
      </trdgrp>
      <trdgrp lng="nl">
        <trd>klooster<klr tip="ind"> (voor nonnen)</klr></trd>,
        <trd>nonnenklooster</trd>
      </trdgrp>
      <trd lng="ru">&c_m;&c_o;&c_n;&c_a;&c_s;&c_t;&c_y;&c_r;&c_mol;<klr> (&c_zh;&c_je;&c_n;&c_s;&c_k;&c_i;&c_j;)</klr></trd>
    </drv>

    <drv mrk="monahx.0ismo">
      <kap><tld/>ismo</kap>
      <uzo tip="fak">REL</uzo>
      <snc>
    	<dif>
          Religia vivmaniero, aperinta en <ref cel="buda.0ismo">budaismo</ref>
          kaj transprenita de la tradicia (neprotestanta)
          <ref cel="krist.0anismo">kristanismo</ref>,
          kies karakterizaj trajtoj estas forlaso de la laika socio,
          rompo de la sociaj rilatoj, fordono de la
          hava&jcirc;oj, sindeteno de la seksaj rilatoj; plej ofte, ali&gcirc;o al
          <tld/>a asocio &dash; <tld/>(in)ejo.
    	</dif>
      </snc>
      <trd lng="be">&c_m;&c_a;&c_n;&c_a;&c_s;&c_t;&c_v;&c_a;</trd>
      <trdgrp lng="de">
        <trd>M&ouml;nchstum</trd>
      </trdgrp>
      <trdgrp lng="en">
    	<trd>monachism</trd>,
    	<trd>monasticism</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>szerzetess&eacute;g</trd>,
        <trd>szerzetesi &eacute;letforma</trd>
      </trdgrp>
      <trd lng="nl">monniksdom</trd>
      <trd lng="ru">&c_m;&c_o;&c_n;&c_a;&c_sh;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
    </drv>

    <drv mrk="monahx.0ejestro">
      <kap><tld/>ejestro</kap>
      <uzo tip="fak">REL</uzo>
      <snc>
    	<dif>
          Estro de <tld/>ejo.
    	</dif>
	<rim>
          La&ubreve; la konfesioj kaj tradicioj estas diversaj specialaj
          titoloj:
          <refgrp tip="sub">
            <ref cel="abat.0o">abato</ref>,
            <ref cel="arkimandrit.0o">arkimandrito</ref>,
            <ref cel="igumen.0o">igumeno</ref>,
            <ref cel="prior.0o">prioro</ref>
          </refgrp>
          ktp.
        </rim>
      </snc>
      <trd lng="be">&c_n;&c_a;&c_s;&c_t;&c_a;&c_ja;&c_c;&c_je;&c_l;&c_mol; <klr tip="amb">(&c_m;&c_a;&c_n;&c_a;&c_s;&c_t;&c_y;&c_r;&c_a;)</klr></trd>
      <trd lng="de">Abt</trd>
      <trd lng="en"><klr tip="amb">(Father) </klr><ind>Superior</ind></trd>
        <trdgrp lng="hu">
          <trd>kolostorf&odblac;n&ouml;k</trd>,
          <trd>rendh&aacute;zf&odblac;n&ouml;k</trd>
        </trdgrp>
        <trdgrp lng="nl">
          <trd>abt</trd>,
          <trd>prior</trd>
        </trdgrp>
      <trd lng="ru">&c_n;&c_a;&c_s;&c_t;&c_o;&c_ja;&c_t;&c_je;&c_l;&c_mol;<klr tip="amb"> &c_m;&c_o;&c_n;&c_a;&c_s;&c_t;&c_y;&c_r;&c_ja;</klr></trd>
    </drv>

    <drv mrk="monahx.0inejestrino">
      <kap><tld/>inejestrino</kap>
      <uzo tip="fak">REL</uzo>
      <snc>
    	<dif>
          Estrino de <tld/>inejo.
    	</dif>
      </snc>
      <trd lng="be">&c_n;&c_a;&c_s;&c_t;&c_a;&c_ja;&c_c;&c_je;&c_l;&c_mol;&c_k;&c_a; <klr tip="amb">(&c_m;&c_a;&c_n;&c_a;&c_s;&c_t;&c_y;&c_r;&c_a;)</klr></trd>
      <trd lng="de">&Auml;btissin</trd>
      <trd lng="en"><klr tip="amb">(Mother) </klr><ind>Superior</ind></trd>
      <trd lng="es">madre superiora</trd>
      <trdgrp lng="hu">
        <trd>z&aacute;rdaf&odblac;n&ouml;k</trd>,
        <trd>ap&aacute;caf&odblac;n&ouml;k</trd>,
        <trd>ap&aacute;tn&odblac;</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>abdis</trd>,
        <trd>priores</trd>
      </trdgrp>
      <trd lng="ru">&c_n;&c_a;&c_s;&c_t;&c_o;&c_ja;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_c;&c_a;<klr tip="amb"> (&c_zh;&c_je;&c_n;&c_s;&c_k;&c_o;&c_g;&c_o;) &c_m;&c_o;&c_n;&c_a;&c_s;&c_t;&c_y;&c_r;&c_ja;</klr></trd>
    </drv>

    <drv mrk="monahx.pastro0o">
      <kap>pastro<tld/>o</kap>
      <uzo tip="fak">KRI</uzo>
      <snc>
    	<dif>
          <tld lit="M"/>o kiu anka&ubreve; estas
	  <ref tip="super" cel="pastr.0o.presbitero">pastro</ref>
	  (pastrigita <tld/>o a&ubreve; mona&hcirc;i&gcirc;inta pastro).
    	</dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_ib;&c_je;&c_r;&c_a;&c_m;&c_a;&c_n;&c_a;&c_h;</trd>,
        <trd>&c_m;&c_a;&c_n;&c_a;&c_h;-&c_s;&c_mol;&c_v;&c_ja;&c_t;&c_a;&c_r;</trd>
      </trdgrp>
      <trd lng="de">M&ouml;nchspriester</trd>
      <trdgrp lng="en">
	<trd>monk-presbyter</trd>,
	<trd>hieromonk</trd>
      </trdgrp>
      <trd lng="grc">&jota_densa;&epsilon;&rho;&omikron;&my;&omikron;&ny;&alfa;&chi;&omikron_ton;&sigma_fina;</trd>
      <trd lng="hu">szerzetespap</trd>
      <trd lng="nl">monnikspriester</trd>
      <trdgrp lng="ru">
	<trd>&c_i;&c_je;&c_r;&c_o;&c_m;&c_o;&c_n;&c_a;&c_h;</trd>,
	<trd>&c_s;&c_v;&c_ja;&c_shch;&c_je;&c_n;&c_n;&c_o;&c_i;&c_n;&c_o;&c_k;</trd>
      </trdgrp>
    </drv>

  </art>
