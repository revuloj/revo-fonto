<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mona&hcirc;</rad>/o
</kap>
<drv mrk="monahx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">REL</uzo>
  <snc>
    <dif>
      Ano de religia ordeno a&ubreve; kongregacio, submetita al specialaj reguloj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>M&ouml;nch</trd>,
    <trd>Nonne</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>monk <klr>(mona&hcirc;o)</klr></trd>,
    <trd>abbey <klr>(mona&hcirc;ino)</klr></trd>
  </trdgrp>
  <trd lng="nl">monnik</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_n;&c_a;&c_h;</trd>,
    <trd>&c_i;&c_n;&c_o;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="monahx.0ejo">
  <kap><ofc>*</ofc><tld/>(in)ejo</kap>
  <uzo tip="fak">REL</uzo>
  <snc>
    <dif>
      Aparta domo, kie kunvivas <tld/>(in)oj.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>monastery <klr>(mona&hcirc;ejo)</klr></trd>,
    <trd>abbey <klr>(mona&hcirc;inejo)</klr></trd>
  </trdgrp>
  <trd lng="nl">klooster</trd>
  <trd lng="ru">&c_m;&c_o;&c_n;&c_a;&c_s;&c_t;&c_y;&c_r;&c_mol;</trd>
</drv>
</art>
