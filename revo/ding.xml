<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: ding.xml,v 1.4 2023/11/04 14:29:02 afido Exp $">
    <kap>
      <rad>ding</rad>/o
      <fnt><bib>PIV</bib></fnt>
    </kap>

<drv mrk="ding.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Sova&gcirc;i&gcirc;inta <ref tip="super" cel="hund.0o">hundo</ref>,
      vivanta en a&ubreve;stralio kaj sudorienta azio
      (<trd lng="la">Canis lupus dingo</trd>):
      <ekz>
        ho! kia besteto &ndash; anta&ubreve;aj piedoj de <tld/>o, malanta&ubreve;aj piedoj de nana kanguruo
        <fnt>
          <bib>Monato</bib>
          <aut>Angela Tellier</aut>
          <vrk><url ref="&Monato;/2004/008306.html">La&ubreve;proverbaj fabeloj</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_d;&c_y;&c_n;&c_g;&c_a; <klr>(&c_d;&c_z;&c_ib;&c_k;&c_ib; &c_s;&c_a;&c_b;&c_a;&c_k;&c_a;)</klr></trd>
    <trd lng="de">Dingo</trd>
    <trd lng="en">dingo</trd>
    <trd lng="fr">dingo <klr>(chien sauvage)</klr></trd>
    <trd lng="ru">&c_d;&c_i;&c_n;&c_g;&c_o;</trd>
  </snc>
  <trd lng="zh">&#x6FB3;&#x6D32;&#x91CE;&#x72AC; <pr>&#xE0;ozh&#x14D;uy&#x11B;qu&#x1CE;n</pr></trd>
</drv>

  </art>

