</drv>

<drv mrk="trakt.mis0i">
  <kap>mis<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="trakt.mis0i.KOMUNE">
    <dif>
      Senzorge, mallerte, difekte <tld/>i:
      <ekz>
        &scirc;ia pse&ubreve;dopatro fimis<tld/>is &scirc;in
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2004/008131.html">Bone prezentita dramo
          malkvietiga</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        se oni neglektas a&ubreve; iel mis<tld/>as &gcirc;in, &gcirc;i malsani&gcirc;as
        <fnt>
          <aut>Hori Yasuo</aut>: <vrk>&leftquot;Purikula&rightquot; piko</vrk>,
          <bib>Monato</bib>, <lok>1997:8, p. 13a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Francesco</nom> <klr>[&#8230;]</klr> en la dramo malagnoskas siajn infanojn, torturas,
        sklavigas, kaj mis<tld/>as ilin
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2004/008131.html">Bone prezentita dramo
          malkvietiga</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        raportoj pri mis<tld/>o de eksterlandanoj
        <fnt>
          <aut>Garbhan MacAoidh</aut>
            <vrk><url ref="&Monato;/2001/007319.php">Ne tiom negative ...</url></vrk>
            <bib>Monato</bib>
            <lok>2001/02, p. 14</lok>
          </fnt>;
      </ekz>
      <ekz>
        mis<tld/>o de homaj rajtoj kaj religia libereco
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2005/008530.php">Konkeri korojn kaj
          animojn</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">misshandeln</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_r;&c_e;&c_n;&c_n;&c_a; <ind>&c_a;&c_b;&c_y;&c_h;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_c;&c_a;</ind></trd>,
    <trd>&c_p;&c_r;&c_y;&c_g;&c_n;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">&scaron;patn&ecaron; zach&aacute;zet</trd>
  <trdgrp lng="en">
    <trd>mistreat</trd>,
    <trd>abuse</trd>
  </trdgrp>
  <trd lng="es">maltratar</trd>
  <trdgrp lng="fr">
    <trd>brimer</trd>,
    <trd>maltraiter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>rosszul <ind>b&aacute;nik</ind></trd>,
    <trd>b&aacute;nt</trd>,
    <trd>b&aacute;ntalmaz</trd>
    <trd>elint&eacute;z <klr tip="ind">(pej.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#34384;&#x5F85;&#12377;&#x308B;  <pr>&#x304E;&#12419;&#x304F;&#x305F;&#12356;&#12377;&#x308B;</pr></trd>,
    <trd>&#37239;&#x4F7F;&#12377;&#x308B;  <pr>&#12371;&#x304F;&#12375;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="pl">zn&eogonek;ca&cacute; si&eogonek;</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_je;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_g;&c_r;&c_u;&c_b;&c_o; &c_o;&c_b;&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_l;&c_o;&c_h;&c_o; &c_o;&c_b;&c_r;&c_a;&c_shch;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">zle zaobch&aacute;dza&tcaron;</trd>
</drv>

<drv mrk="trakt.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="trakt.pri0i.KOMUNE">
    <dif>
      <tld lit="T"/>i pri:
      <ekz>
        &ccirc;i tie ne estas la &gcirc;usta loko por pri<tld/>ado de sciencaj demandoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        britaj doganistoj komencis suspekte pri<tld/>i &ccirc;iun &scirc;ar&gcirc;a&ubreve;ton el Litovio
        <fnt>
	<aut>Laimius Stra&zcaron;nickas</aut>
	<vrk><url ref="&Monato;/2000/006830.php">Kontrabanditaj cigaredoj invadas Brition</url></vrk>
	<bib>Monato</bib>
	<lok>2000/04, p. 13</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi &ccirc;esigas mian pri<tld/>adon por respondi al demando
        <fnt>
       <aut>Jean-Yves Santerre</aut>
          <vrk><url ref="&Monato;/2000/006864.php">&Ccirc;evalrasoj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/05, p. 18</lok>
        </fnt>;
      </ekz>
      <ekz>
        en sciencaj pri<tld/>oj oni zorge distenu koron kaj cerbon
        <fnt>
        <aut>Manuel Halvelik</aut>
          <vrk><url ref="&Monato;/1993/A931102.php">Kaj la suno haltis ...</url></vrk>
          <bib>Monato</bib>
          <lok>1993/11, p. 23</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio por li estas nepri<tld/>inda normala&jcirc;o
      <fnt>
        <aut>Michel Duc Goninaz</aut>
        <vrk><url ref="&Monato;/2000/006947.html">Eks-Jugoslavio:
          kiu kiun mistifikas?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 8</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">behandeln</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_g;&c_l;&c_ja;&c_d;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_m;&c_ja;&c_r;&c_k;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
