      <ref cel="diskut.0i">diskuti</ref>
    </refgrp>
    <trd lng="de">verhandeln</trd>
  </snc>
  <trd lng="be">&c_v;&c_je;&c_s;&c_mol;&c_c;&c_ib; <ind>&c_p;&c_je;&c_r;&c_a;&c_m;&c_o;&c_v;&c_y;</ind> <klr>(&c_p;&c_a;&c_m;&c_ib;&c_zh;&c_s;&c_o;&c_b;&c_k;&c_u;)</klr></trd>
