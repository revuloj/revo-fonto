  <trd lng="ja">&#21332;&#x8B70;&#12377;&#x308B;  <pr>&#x304D;&#12423;&#12358;&#x304E;&#12377;&#x308B;</pr></trd>
  <trdgrp lng="pl">
    <trd>rokowa&cacute;</trd>,
    <trd>paktowa&cacute;</trd>,
