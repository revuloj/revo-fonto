<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hasan.xml,v 1.6 2023/11/11 21:20:41 revo Exp $">
<kap>
  <rad>Hasan</rad>/o
</kap>
<drv mrk="hasan.0o">
  <kap><tld/>o</kap>
  <snc mrk="hasan.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> arabdevena <klr>(&leftquot;la bona&rightquot;)</klr>:
      <ekz>
        en sia deka jaro li venkis la grizbarban
        <tld/>on, kiu &gcirc;is tiam havis la famon de
        plej bona &scirc;akludanto
        <fnt>
          <aut>F. Karinthy, trad. K. Bod&oacute;</aut>,
          <vrk>Cezaro kaj Abu Kair</vrk>,
          <lok>en: <url
          ref="http://i-espero.info/files/elibroj/eo%20-%20karinthy,%20frigyes%20-%20norda%20vento.pdf">
          Norda Vento</url>, 1926</lok>
        </fnt>;
      </ekz>
      <!--
      <ekz>
        <tld/> deziris al la birdo longecon de plumoj,
        kaj nomis &gcirc;in Rok
        <fnt>
          <aut>Multatuli, trad. W. F. Pilger</aut>,
          <vrk><url
          ref="http://pilger.home.xs4all.nl/pilger/hasan1.htm">
          Na&ubreve;a Historio pri A&ubreve;toritato</url></vrk>,
          <lok>1996</lok>
        </fnt>;
      </ekz>
      -->
      <ekz>
        <tld/>o prelegis pri la Historio de <nom>ANEB</nom>
        <fnt>
          <aut>Ntahonsigaye H.</aut>,
          <vrk><url ref="http://ilei.info/novajxoj/raporto_bubanza.php">
          Raporto pri la seminario en Bubanza</url></vrk>,
          <lok>[2010?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ar">&a_H;&a_s;&a_n;</trd>
  <trd lng="be">&c_H;&c_a;&c_s;&c_a;&c_n;</trd>
  <trd lng="de">Hassan</trd>
  <trd lng="fr">Hassan</trd>
  <trd lng="nl">Hassan</trd>
  <trd lng="pl">Hassan</trd>
</drv>
</art>

