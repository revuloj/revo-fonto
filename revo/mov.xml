      <trdgrp lng="nl">
        <trd>paralyseren</trd>,
        <trd>verlammen</trd>
      </trdgrp>
      <trd
        lng="ru">&c_o;&c_b;&c_je;&c_z;&c_d;&c_v;&c_i;&c_zh;&c_i;&c_t;&c_mol;</trd>
