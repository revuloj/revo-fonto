    <trd><ind>bewegt</ind> werden</trd>
  </trdgrp>
  <trd lng="en">move</trd>
  <trdgrp lng="es">
    <trd><ind>mover</ind>se<klr> (intr.)</klr></trd>,
    <trd><ind>desplazar</ind>se</trd>
  </trdgrp>
  <trdgrp lng="fr">
