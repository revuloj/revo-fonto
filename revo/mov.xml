    <drv mrk="mov.propra0e">
      <kap>propra<tld/>e</kap>
      <snc>
        <dif>
          Per propra forto, interna impulso:
          <ekz>
            ili venis al la fera pordego, kiu kondukas en la urbon; kaj &gcirc;i
            malfermi&gcirc;is al ili propra<tld/>e
            <fnt><bib>NT</bib>, <lok>&Ago; 12:10</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="fr">
        <trd>de soi-m&ecirc;me</trd>,
        <trd>de son propre mouvement</trd>,
        <trd>tout seul<klr tip="amb"> (de son propre mouvement)</klr></trd>
      </trdgrp>
    </drv>

    <drv mrk="mov.sen0a">
      <kap>sen<tld/>a</kap>
      <snc>
        <dif>
          Ne<tld/>i&gcirc;anta:
          <ekz>
            li restis sen<tld/>e;
          </ekz><ekz>
            akvon sen<tld/>an kovras putra&jcirc;o
            <fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            sen<tld/>a kiel &scirc;tonego.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_n;&c_je;&c_r;&c_u;&c_h;&c_o;&c_m;&c_y;</trd>,
        <trd>&c_s;&c_t;&c_a;&c_ja;&c_ch;&c_y;</trd>,
        <trd>&c_l;&c_ja;&c_zh;&c_a;&c_ch;&c_y;</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>immobile</trd>
      </trdgrp>
      <trd lng="nl">roerloos</trd>
      <trdgrp lng="ru">
        <trd>&c_n;&c_je;&c_p;&c_o;&c_d;&c_v;&c_i;&c_zh;&c_n;&c_y;&c_j;</trd>,
        <trd>&c_n;&c_je;&c_d;&c_v;&c_i;&c_zh;&c_i;&c_m;&c_y;&c_j;</trd>
      </trdgrp>
    </drv>

    <drv mrk="mov.sen0igi">
