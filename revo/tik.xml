<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tik</rad>/i
</kap>
<drv mrk="tik.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="tik.0i.MED">
    <dif>
      Pretervole tiri iun muskolon:
      <ekz>
        unu el ni okule <tld/>is kaj tro okulfrape rigardis al iu
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>dua libro, &ccirc;apitro 41a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">tiquer<klr tip="amb"> (avoir un tic)</klr></trd>
</drv>

<drv mrk="tik.0o">
  <kap><tld/>o</kap>
  <snc mrk="tik.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Nerva kuntiro de muskoloj, plej ofte tiuj de la viza&gcirc;o.
    </dif>
  </snc>
  <trd lng="de">Tick</trd>
  <trd lng="fr">tic</trd>
  <trdgrp lng="hu">
    <trd>izomr&aacute;ng&aacute;s</trd>,
    <trd>arcr&aacute;ng&aacute;s</trd>,
    <trd>tic</trd>
  </trdgrp>
  <trd lng="nl">tic</trd>
  <trdgrp lng="pt">
    <trd>tique</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_i;&c_k; <klr tip="amb">(&c_n;&c_je;&c_r;&c_v;&c_n;&c_y;&c_j;)</klr></trd>
</drv>
</art>
