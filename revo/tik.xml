<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tik.xml,v 1.19 2023/09/04 19:01:52 revo Exp $">
<kap>
  <rad>tik</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tik.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="tik.0i.MED">
    <dif>
      Pretervole <ref tip="super" cel="tir.0i.al_si">tiri</ref> iun muskolon:
      <ekz>
        unu el ni okule <tld/>is kaj tro okulfrape rigardis al iu
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>dua libro, &ccirc;apitro
        41a</lok></fnt>;
      </ekz>
      <ekz>
        kelkan tempon la gasto silente mal&gcirc;ojis kaj <tld/>adis, sed finfine li ekparolis
        <fnt>
          <bib>MkM</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="trem.0eri">tremeri</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_je;&c_c;&c_mol; <ind>&c_c;&c_ib;&c_k;</ind></trd>,
    <trd>&c_p;&c_a;&c_k;&c_u;&c_t;&c_v;&c_a;&c_c;&c_mol; &c_n;&c_a; <ind>&c_c;&c_ib;&c_k;</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>m&iacute;t tik</trd>,
    <trd>m&iacute;t &scaron;kub&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">zucken</trd>
  <trd lng="fr">tiquer<klr tip="amb"> (avoir un tic)</klr></trd>
  <trdgrp lng="pl">
    <trd>mie&cacute; <ind>tik</ind></trd>,
    <trd>mie&cacute; <ind>kurcz</ind></trd>,
    <trd>mie&cacute; <ind>skurcz</ind></trd>,
  </trdgrp>
  <trdgrp lng="sk">
    <trd>cuka&tcaron;</trd>,
    <trd>myka&tcaron;</trd>,
    <trd>po&scaron;klb&aacute;va&tcaron; <klr>(z nervozity)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="tik.0o">
  <kap><tld/>o</kap>
  <snc mrk="tik.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Nerva <ref tip="super" cel="tir.kun0o.KOMUNE">kuntiro</ref> de muskoloj, plej ofte tiuj de la
      viza&gcirc;o:
      <ekz>
        <nom>Plum</nom> absorbis gluton da viskio, gratis al si la nukon, dum spasma tiko
        manifesti&gcirc;is &ccirc;e lia lipangulo
        <fnt>
          <bib>CKv</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_c;&c_ib;&c_k; <klr tip="amb">(&c_n;&c_e;&c_r;&c_v;&c_o;&c_v;&c_y;)</klr></trd>
    <trd lng="de">Zuckung</trd>
  </snc>
  <snc mrk="tik.0o.manio">
    <dif>
      <ref tip="dif" cel="mani.0o">Manio</ref>:
      <ekz>
        la juna oficiro havis <tld/>on, kiam li estis priokupata a&ubreve; profunde
        meditadanta, nevole kaj senpripense li prenis sian maldekstran polekson inter
        la dika kaj la montra fingroj de la dekstra mano, kaj &gcirc;in rapide kaj sen&ccirc;ese
        faldis kaj malfaldis la&ubreve; ne interrompita maniero
        <fnt>
          <bib>ChL</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_a;&c_n;&c_ib;&c_ja;</trd>
    <trd lng="de">Tick</trd>
  </snc>
  <rim mrk="tik.0o.HOM">    
    <ref tip="hom" cel="tik1.0o.TEKS">Tiko</ref>
    anka&ubreve; estas speco de &scirc;tofo.
  </rim>  
  <trd lng="es">tic</trd> 
  <trd lng="fr">tic</trd>
  <trdgrp lng="hu">
    <trd>izomr&aacute;ng&aacute;s</trd>,
    <trd>arcr&aacute;ng&aacute;s</trd>,
    <trd>tic</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30C1;&#x30C3;&#x30AF;</trd>,
    <trd>&#x3051;&#x3044;&#x308C;&#x3093;</trd>
  </trdgrp>
  <trd lng="nl">tic</trd>
  <trdgrp lng="pl">
    <trd>nerwowy <ind>kurcz</ind><klr> (zazw. twarzy)</klr></trd>,
    <trd>tik</trd>,
    <trd>skurcz nerwowy</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>tique</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_i;&c_k; <klr tip="amb">(&c_n;&c_je;&c_r;&c_v;&c_n;&c_y;&c_j;)</klr></trd>
  <trd lng="sk">nerv&oacute;zne mykanie svalu</trd>
  <trdgrp lng="cs">
    <trd>tic</trd>,
    <trd>tik</trd>,
    <trd>&scaron;kub&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="uk">&c_t;&c_i;&#x43A;</trd>
</drv>
</art>
