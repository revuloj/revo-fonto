<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>cipres</rad>/o
</kap>
<drv mrk="cipres.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="cipres.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Cupressus</trd>), rezina piramidforma pinglofolia arbo.
    </dif>
  </snc>
  <trd lng="de">Zypresse</trd>
  <trd lng="ru">&c_k;&c_i;&c_p;&c_a;&c_r;&c_i;&c_s;</trd>
</drv>
<drv mrk="cipres.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio el pinglofoliaj arboj, al kiuj apartenas ekz.
      <refgrp tip="sub">
        <ref cel="cipres.0o">cipreso</ref> kaj
        <ref cel="juniper.0o">junipero</ref>.
      </refgrp>
    </dif>
  </snc>
  <trd lng="de">Zypressengew&auml;chse</trd>
</drv>
</art>
