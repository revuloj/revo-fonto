<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>cipres</rad>/o
</kap>
<drv mrk="cipres.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="cipres1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Cupressus</trd>), rezina piramidforma pinglofolia arbo.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_p;&c_a;&c_r;&c_y;&c_s;</trd>
  <trdgrp lng="br">
    <trd>siprezenn</trd>,
    <trd>gwezenn-siprez</trd>
  </trdgrp>
  <trd lng="de">Zypresse</trd>
  <trd lng="el">&kappa;&ypsilon;&pi;&alfa;&rho;&jota_ton;&sigma;&sigma;&jota;</trd>
  <trd lng="fr">cypr&egrave;s</trd>
  <trdgrp lng="hu">
    <trd>ciprusfa</trd>,
    <trd>val&oacute;di <ind>ciner&aacute;ria</ind></trd></trdgrp>
  <trd lng="pt">cipreste</trd>
  <trd lng="ru">&c_k;&c_i;&c_p;&c_a;&c_r;&c_i;&c_s;</trd>
</drv>

</art>
