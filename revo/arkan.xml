<kap>
  <rad>arkan</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="arkan.0o">
  <kap><tld/>o</kap>
  <snc mrk="arkan.0o.SPO">
    <dif>
      Iu el la <ref tip="malprt" cel="kart.0o.SPO">kartoj<sncref/></ref>
      de <ref tip="malprt" cel="tarok.0o">taroko</ref>:
      <ekz>
        la ordo de la ma&jcirc;oraj <tld/>oj estas la&ubreve; la ordo en RWS tarokoj, kaj la minoraj
        <tld/>oj portas bildojn, ne nur emblemojn
        <fnt>
          <aut>Joey JoJo</aut>
          <vrk><url ref="https://medium.com/@Joe_Fumai/recenzo-de-kartaro-la-flika-taroko-the-patch-tarot-27f436f770d">Recenzo
          de Kartaro &mdash; La Flika Taroko</url></vrk>
          <lok>2017-12-19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    (pri sekreta instruo a≈≠ rito)
    la&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>mistero</ke>.
  </rim>

  <trd lng="be">&c_a;&c_r;&c_k;&c_a;&c_n;&c_a; <klr>(&c_k;&c_a;&c_r;&c_t;&c_a; &c_t;&c_a;&c_r;&c_o;)</klr></trd>
  <trd lng="fr">arcane</trd>
  <trd lng="pl">karta tarota</trd>
  <trdgrp lng="uk">
    <trd>&c_a;&c_r;&#1082;&c_a;&#1085;&c_a;</trd>,
    <trd>&c_d;&c_i;&#1087;&#1083;&#1086;&#1084;&c_a;&c_t;&c_i;&c_ch;&#1085;&c_a;&#160;&c_t;&c_a;&c_jeu;&#1084;&#1085;&c_i;&c_c;&#1103;</trd>
  </trdgrp>
</drv>
</art>

