<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: advoka.xml,v 1.42 2023/10/12 16:57:42 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>advokat</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="advoka.0o">
  <kap><tld/>o</kap>
  <snc mrk="advoka.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Persono oficiale rajtigita por defendi en ju&gcirc;ejo la interesojn
      de procesanto a&ubreve; akuzato:
      <ekz>
        la &ccirc;efpastro <nom>Ananias</nom> malsupreniris tien kun kelkaj plia&gcirc;uloj, kaj unu <tld/>o,
        nomata Tertulo kaj ili faris denuncon anta&ubreve; la provincestro kontra&ubreve; Pa&ubreve;lo
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 24:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        devas &ccirc;i tie pasi ri&ccirc;a grafo el Regensburgo, kiu gajnis proceson pri miliono per
        la ruza&jcirc;oj de sia <tld/>o
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        li rigardis la du <tld/>ojn kiel kunka&ubreve;zantoj de lia stato <klr>[&#x2026;]</klr> kaj
        forte ilin malestimis
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        le&gcirc;o estas bona se <tld/>o &gcirc;in helpas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        pastro vivas de pre&gcirc;oj <tld/>o de le&gcirc;oj
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Verteidiger</trd>,
      <trd>Anwalt</trd>,
      <trd>Advokat [arkaika]</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>barrister</trd>,
      <trd>attorney</trd>,
      <trd>lawyer</trd>
    </trdgrp>
    <trd lng="es">abogado</trd>
    <trdgrp lng="fr">
      <trd>avocat<klr tip="ind"> (juriste)</klr></trd>,
      <trd>d&eacute;fenseur<klr tip="ind"> (juriste)</klr></trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>peng<ind>acara</ind></trd>,
      <trd>advokat</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>avvocato</trd>,
      <trd>difensore<klr> (avvocato)</klr></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>advogado</trd>,
      <trd>defensor</trd>
    </trdgrp>
    <trd lng="sk">advok&aacute;t</trd>
    <trd lng="sv">advokat</trd>
  </snc>
  <snc mrk="advoka.0o.porparolanto">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="parol.por0anto.advokato">Porparolanto</ref>:
      <ekz>
        estu mia <tld/>o anta&ubreve; mia edzino;
      </ekz>
      <ekz>
        <tld/>oj de eleksterismo
        <fnt>
          <bib>BonaLingvo</bib>
          <lok>6. Ies Malfido, Fido Mia</lok>
        </fnt>;
      </ekz>
      <ekz>
        permesu, ke mi por momento ludu la rolon de diablo-<tld/>o
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 7</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="apolog1.0o">apologiisto</ref>,
      <ref cel="pled.0i">pledi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>F&uuml;rsprecher</trd>
    </trdgrp>
    <trd lng="en">advocate</trd>
    <trdgrp lng="fr">
      <trd>avocat<klr tip="ind"> (intercesseur)</klr></trd>,
      <trd>d&eacute;fenseur<klr tip="ind"> (intercesseur)</klr></trd>,
      <trd>intercesseur</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>avvocato<klr> (fig.)</klr></trd>,
      <trd>difensore<klr> (intercessore)</klr></trd>,
      <trd>intercessore</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_v;&c_a;&c_k;&c_a;&c_t;</trd>,
    <trd>&c_a;&c_b;&c_a;&c_r;&c_o;&c_n;&c_c;&c_a; <klr>(&c_s;&c_u;&c_d;&c_o;&c_v;&c_y;)</klr></trd>
  </trdgrp>
  <trd lng="bg">&c_a;&c_d;&c_v;&c_o;&c_k;&c_a;&c_t;</trd>
  <trd lng="bo">&t_kha;&t_rata;&t_gigu;&t_ma;&t_sa;&t_tsheg;&t_ra;&t_tsata;&t_naro;&t_da;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>advok&aacute;t</trd>,
    <trd>obh&aacute;jce</trd>,
    <trd>pr&aacute;vn&iacute; z&aacute;stupce</trd>
  </trdgrp>
  <trd lng="de">Anwalt</trd>
  <trd lng="he">&ayin;&vav;&resh;&fkaf; &dalet;&yod;&fnun;</trd>
  <trd lng="hu">&uuml;gyv&eacute;d</trd>
  <trdgrp lng="ja">
    <trd>&#x5F01;&#x8B77;&#x58EB;  <pr>&#x3079;&#x3093;&#x3054;&#x3057;</pr></trd>,
    <trd>&#x5F01;&#x8B77;&#x4EBA;  <pr>&#x3079;&#x3093;&#x3054;&#x306B;&#x3093;</pr></trd>,
    <trd>&#x4EE3;&#x5F01;&#x8005;  <pr>&#x3060;&#x3044;&#x3079;&#x3093;&#x3057;&#x3083;</pr></trd>
  </trdgrp>
  <trd lng="nl">advocaat</trd>
  <trdgrp lng="pl">
    <trd>adwokat</trd>,
    <trd>obro&nacute;ca</trd>
  </trdgrp> 
  <trdgrp lng="ro">
    <trd>avocat</trd>,
    <trd>campion</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_d;&c_v;&c_o;&c_k;&c_a;&c_t;</trd>,
    <trd>&c_z;&c_a;&c_shch;&c_i;&c_t;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>advok&aacute;t, obhajca</trd>,
    <trd>obhajca</trd>
  </trdgrp>
  <trd lng="sv">f&ouml;rsvarare</trd>
  <trdgrp lng="uk">
    <trd>&c_a;&c_d;&c_v;&c_o;&c_k;&c_a;&c_t;</trd>,
    <trd>&c_z;&c_a;&c_h;&c_i;&c_s;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5F8B;&#x5E08;</trd>,
    <trd><klr>tr. </klr>&#x5F8B;&#x5E2B;<klr> [l&#x1DC;sh&#x12B;]</klr></trd>
  </trdgrp>
</drv>

<drv mrk="advoka.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="afer.jugx0o">Ju&gcirc;afero</ref>, en kiu <tld/>o defendas:
      <ekz>
        tiel malluma, kiel <klr>[&#x2026;]</klr> la <tld/>a&jcirc;o entute
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        lasu do trankvilaj ciajn <tld/>a&jcirc;ojn, kaj ne malhelpu
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sepa</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kaz.0o.afero">kazo<sncref/></ref>,
<ref cel="proces.0o.JUR">proceso<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Gerichtssache</trd>,
    <trd>Fall <klr>(Gericht)</klr></trd>
  </trdgrp>
</drv>


</art>
