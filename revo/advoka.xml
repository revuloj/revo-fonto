<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: advoka.xml,v 1.36 2021/05/28 12:52:36 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>advokat</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="advoka.0o">
  <kap><tld/>o</kap>
  <snc mrk="advoka.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Persono oficiale rajtigita por defendi en ju&gcirc;ejo la interesojn
      de procesanto a&ubreve; akuzato:
      <ekz>
        le&gcirc;o estas bona se <tld/>o &gcirc;in helpas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        pastro vivas de pre&gcirc;oj <tld/>o de le&gcirc;oj
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Verteidiger</trd>,
      <trd>Anwalt</trd>,
      <trd>Advokat [arkaika]</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>barrister</trd>,
      <trd>attorney</trd>,
      <trd>lawyer</trd>
    </trdgrp>
    <trd lng="es">abogado</trd>
    <trdgrp lng="fr">
      <trd>avocat<klr tip="ind"> (juriste)</klr></trd>,
      <trd>d&eacute;fenseur<klr tip="ind"> (juriste)</klr></trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>peng<ind>acara</ind></trd>,
      <trd>advokat</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>avvocato</trd>,
      <trd>difensore<klr> (avvocato)</klr></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>advogado</trd>,
      <trd>defensor</trd>
    </trdgrp>
    <trd lng="sk">advok&aacute;t</trd>
    <trd lng="sv">advokat</trd>
  </snc>
  <snc mrk="advoka.0o.porparolanto">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="parol.por0anto.advokato">Porparolanto</ref>:
      <ekz>
        estu mia <tld/>o anta&ubreve; mia edzino<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Anwalt</trd>
    </trdgrp>
    <trd lng="en">advocate</trd>
    <trdgrp lng="fr">
      <trd>avocat<klr tip="ind"> (intercesseur)</klr></trd>,
      <trd>d&eacute;fenseur<klr tip="ind"> (intercesseur)</klr></trd>,
      <trd>intercesseur</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>avvocato<klr> (fig.)</klr></trd>,
      <trd>difensore<klr> (intercessore)</klr></trd>,
      <trd>intercessore</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_v;&c_a;&c_k;&c_a;&c_t;</trd>,
    <trd>&c_a;&c_b;&c_a;&c_r;&c_o;&c_n;&c_c;&c_a; <klr>(&c_s;&c_u;&c_d;&c_o;&c_v;&c_y;)</klr></trd>
  </trdgrp>
  <trd lng="bg">&c_a;&c_d;&c_v;&c_o;&c_k;&c_a;&c_t;</trd>
  <trd lng="bo">&#xF41;&#xFB2;&#xF72;&#xF58;&#xF66;&#xF0B;&#xF62;&#xFA9;&#xF7C;&#xF51;&#xF0B;&#xF54;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>advok&aacute;t</trd>,
    <trd>obh&aacute;jce</trd>,
    <trd>pr&aacute;vn&iacute; z&aacute;stupce</trd>
  </trdgrp>
  <trd lng="de">Anwalt</trd>
  <trd lng="he">&#x5E2;&#x5D5;&#x5E8;&#x5DA; &#x5D3;&#x5D9;&#x5DF;</trd>
  <trd lng="hu">&uuml;gyv&eacute;d</trd>
  <trd lng="nl">advocaat</trd>
  <trdgrp lng="pl">
    <trd>adwokat</trd>,
    <trd>obro&nacute;ca</trd>
  </trdgrp> 
  <trdgrp lng="ro">
    <trd>avocat</trd>,
    <trd>campion</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_d;&c_v;&c_o;&c_k;&c_a;&c_t;</trd>,
    <trd>&c_z;&c_a;&c_shch;&c_i;&c_t;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>advok&aacute;t, obhajca</trd>,
    <trd>obhajca</trd>
  </trdgrp>
  <trd lng="sv">f&ouml;rsvarare</trd>
  <trdgrp lng="zh">
    <trd>&#x5F8B;&#x5E08;</trd>,
    <trd><klr>tr. </klr>&#x5F8B;&#x5E2B;<klr> [l&#x1DC;sh&#x12B;]</klr></trd>
  </trdgrp>
</drv>
</art>
