<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: solipsist.xml,v 1.5 2023/09/30 20:23:19 revo Exp $">
<kap>
  <rad>solipsist</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="solipsist.0o">
  <kap><tld/>o</kap>
  <snc mrk="solipsist.0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      <ref tip="vid" cel="solipsism.0o.FIL">Solipsism</ref>ano:
      <ekz>
        mi, vere, ne estas <tld/>o, sed de tempo al tempo havas
        senton, kvaza&ubreve; tie, kie mi ne estas <klr>...</klr>
        &ccirc;io frosti&gcirc;as
        <fnt>
          <aut>S. Lem, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Lem/index.html">
          Surloka Esploro</url></vrk>,
          <lok>[2003]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_l;&c_ib;&c_p;&c_s;&c_y;&c_s;&c_t;</trd>
  <trd lng="en">solipsist</trd>
  <trd lng="fr">solipsiste<klr> (subst.)</klr></trd>
  <trd lng="pl">solipsysta</trd>
  <trd lng="uk">&c_s;&#x43E;&#x43B;&c_ib;&#x43F;&c_s;&c_i;&c_s;&c_t;</trd>
</drv>
</art>

