<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>traf</rad>/i
</kap>
<drv mrk="traf.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="klr">(iu, io konkreta)</uzo>
    <dif>
      Atingi per &jcirc;etita objekto a&ubreve; per sia propra
      movi&gcirc;anta korpo ion
      celitan:
      <ekz>
        <tld/>i iun per kuglo;
      </ekz>
      <ekz>
        la kuglo <tld/>is lin;
      </ekz>
      <ekz>
        vidas okulo, sed mano ne <tld/>as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind><tld/>i du celojn per unu &scirc;tono</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">k&eacute;t legyet &uuml;t egy
        csap&aacute;sra</trd>
        <trd lng="pt">acertar</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i per la parolo rekte en la viza&gcirc;on
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="tusx.0i">tu&scirc;i</ref>.
    </refgrp>
    <trdgrp lng="es">
      <trd>atinar</trd>,
      <trd>alcanzar</trd>,
      <trd>acertar</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>atteindre</trd>,
      <trd>rencontrer</trd>,
      <trd>toucher<klr> (un but)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>tal&aacute;l</trd>,
      <trd>eltal&aacute;l</trd>
    </trdgrp>
    <trd lng="pl">trafia&cacute;</trd>
    <trd lng="ru">&c_p;&c_o;&c_p;&c_a;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <uzo tip="klr">(f. io abstrakta)</uzo>
    <dif>
      Atingi:
      <ekz>
        la mateno <tld/>is ilin en Hebron<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la morto <tld/>is lin subite;
      </ekz>
      <ekz>
        mi ne hontis konfesi la trompi&gcirc;ojn kiuj min
        <tld/>is<fnt>Z</fnt>;
      </ekz>
      <ekz>
        malfeli&ccirc;o <tld/>is lin<fnt>Z</fnt>;
      </ekz>
      <ekz>
        multe da suferoj kaj mizero <tld/>is tiam la
        hebreojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        stranga okazo min <tld/>is<fnt>Z</fnt>;
      </ekz>
      <ekz>
        neniu scias, kio morga&ubreve; lin <tld/>os
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        terura estis la vida&jcirc;o <tld/>anta liajn okulojn.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="frap.0i">frapi</ref>.
    </refgrp>
    <trdgrp lng="es">
      <trd>alcanzar</trd>,
      <trd>encontrar</trd>
    </trdgrp>
    <trd lng="hu">&eacute;r <klr tip="ind">(hat r&aacute;)</klr></trd>
    <trdgrp lng="pl">
      <trd>przyda&zdot;a&cacute;<klr> si&eogonek;</klr></trd>,
      <trd>spotyka&cacute;<klr> (co&sacute; - kogo&sacute;)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_r;&c_a;&c_z;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_n;&c_a;&c_s;&c_t;&c_i;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Renkonti ian konkretan baron; fali 2:
      <ekz>
        <ind><tld/>is fal&ccirc;ilo sur &scirc;tonon</ind>,
        <fnt><bib>PrV</bib></fnt>
        <trd lng="hu">belet&ouml;rt a bicsk&aacute;ja</trd>
        <trd lng="pl">trafi&lstroke;a kosa na kamie&nacute;</trd>
        <trd lng="ru">&c_n;&c_a;&c_sh;&c_l;&c_a; &c_k;&c_o;&c_s;&c_a;
        &c_n;&c_a; &c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;</trd>
      </ekz>
      <ekz>
        al &scirc;tono<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i per la viza&gcirc;o en koton
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        li <tld/>is tripiedon kaj vundis brovon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la veturilo <tld/>is en kavon;
      </ekz>
      <ekz>
        <tld/>i el sub pluvo en riveron<fnt>Z</fnt>,
        <trd lng="pl">trafi&cacute; z deszczu pod rynn&eogonek;</trd>
      </ekz>
      <ekz>
        el la fajro sub flamon;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i en embarason;
      </ekz>
      <ekz>
        kelkaj <tld/>is en la manojn de la polico;
      </ekz>
      <ekz>
        en mia vagado mi <tld/>is tre originalan homon;
      </ekz>
      <ekz>
        celi pavon, <tld/>i kavon<fnt>Z</fnt>
        <klr>(mal<tld/>i tro alte celante)</klr>.
      </ekz>
	</dif>
    <trdgrp lng="es">
      <trd>tropezar</trd>,
      <trd>caer</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>ker&uuml;l <klr tip="ind">(vhova)</klr></trd>,
      <trd>jut <klr tip="ind">(vhova)</klr></trd>
    </trdgrp>	
    <trdgrp lng="pl">
      <trd>trafia&cacute;<klr> na/w co&sacute;</klr></trd>,
      <trd>natrafia&cacute;<klr> na co&sacute;</klr></trd>,
      <trd>natyka&cacute; si&eogonek;<klr> na co&sacute;</klr></trd>,
      <trd>natkn&aogonek;&cacute; si&eogonek;<klr> na
      co&sacute;</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_p;&c_a;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_n;&c_a;&c_t;&c_k;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="klr">(f. iu, io)</uzo>
    <dif>
      Sperti renkonti ion abstraktan:
      <ekz>
        <tld/>i malfeli&ccirc;on, sukceson, malsukceson;
      </ekz>
      <ekz>
        <tld/>i bonan akcepton<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i rezultaton, la &gcirc;ustan vojon, la &gcirc;ustan
        sencon.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>kap</trd>,
      <trd>r&eacute;szes&uuml;l</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>trafia&cacute; si&eogonek;</trd>,
      <trd>przytrafia&cacute; si&eogonek;</trd>,
      <trd>spotyka&cacute; si&eogonek;<klr> (z
      czym&sacute;)</klr></trd>,
      <trd>doznawa&cacute;</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_o;&c_p;&c_a;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="de">treffen</trd>
  <trd lng="en">strike</trd>
  <trdgrp lng="nl">
    <trd>raken</trd>,
    <trd>treffen</trd>,
    <trd>halen<klr> (v.e.trein)</klr></trd>,
    <trd>terecht komen</trd>
  </trdgrp>
</drv>

<drv mrk="traf.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="T"/>anta, &gcirc;uste atinganta la celon:
      <ekz>
        <tld/>a respondo, argumento, rezultato, rimedo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">treffend</trd>
  <trdgrp lng="es">
    <trd>acertado</trd>,
    <trd>acertada</trd>,
    <trd>atinado</trd>,
    <trd>atinada</trd>
  </trdgrp>
  <trd lng="hu">tal&aacute;l&oacute;</trd>  
  <trd lng="nl">treffend</trd>
  <trdgrp lng="pl">
    <trd>trafny</trd>,
    <trd>celny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_t;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_u;&c_d;&c_a;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="traf.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      En tute ta&ubreve;ga kaj alcela maniero:
      <ekz>
        <tld/>e ju&gcirc;i aferon;
      </ekz>
      <ekz>
        <tld/>e kopii ian manieron de parolado kaj irado<fnt>Z</fnt>.
      </ekz>
      <em><tld/>e mal<tld/>e</em><sup>Z</sup>,
    </dif>
  </snc>
  <trd lng="de">treffend</trd>
  <trd lng="hu">tal&aacute;l&oacute;an</trd>  
  <trd lng="nl">juist</trd>
  <trdgrp lng="pl">
    <trd>trafnie</trd>,
    <trd>celnie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_t;&c_k;&c_o;</trd>,
    <trd>&c_u;&c_d;&c_a;&c_ch;&c_n;&c_o;</trd>,
    <trd>&c_t;&c_o;&c_ch;&c_n;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="traf.0prove">
  <kap><tld/>prove</kap>
  <snc>
    <dif>
      La&ubreve;hazarde, la&ubreve;&scirc;ance, blinde je l' <tld/>o.
    </dif>
  </snc>
  <trd lng="hu">tal&aacute;lomra</trd>  
  <trd lng="nl">lukraak</trd>
  <trd lng="pl">na <ind>chybi&lstroke;-trafi&lstroke;</ind></trd>
  <trd lng="ru">&c_n;&c_a;&c_u;&c_g;&c_a;&c_d;</trd>
</drv>

<drv mrk="traf.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kontra&ubreve;vole ne atingi la celita&jcirc;on:
      <ekz>
        kiu rigardas &ccirc;ielon, mal<tld/>as sian celon
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="sukces.mal0i">malsukcesi</ref>,
      <ref cel="fiask.0i">fiaski</ref>,
      <ref cel="rev">disrevi&gcirc;i</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="lepor.0o">leporo</ref>,
      <ref cel="sxton.0o">&scirc;tono</ref>,
      <ref cel="naz.0o">nazo</ref>,
      <ref cel="har">lipharo</ref>..
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>&eacute;chouer</trd>,
    <trd>rater</trd>,
    <trd>manquer</trd>
  </trdgrp>
  <trd lng="de">verpassen</trd>
  <trd lng="es">errar</trd>
  <trdgrp lng="hu">
    <trd>elv&eacute;t</trd>,
    <trd>mell&eacute;tal&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">missen</trd>
  <trdgrp lng="pl">
    <trd>chybi&cacute;</trd>,
    <trd>spud&lstroke;owa&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_m;&c_a;&c_z;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="traf.0povo">
  <kap><tld/>povo</kap>
  <snc>
    <dif>
      Distanco &gcirc;is kiu povas atingi &jcirc;etilo a&ubreve; ia ajn
      pafilo.
    </dif>
  </snc>
  <trd lng="de">Reichweite</trd>
  <trd lng="es">capacidad de <ind>alcance</ind></trd>
  <trdgrp lng="hu">
    <trd>hat&oacute;t&aacute;v</trd>,
    <trd>hat&oacute;sug&aacute;r</trd>
  </trdgrp>
  <trd lng="nl">reikwijdte</trd>
  <trdgrp lng="pl">
    <trd>zasi&eogonek;g</trd>
  </trdgrp>
  <trd
  lng="ru">&c_d;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_b;&c_o;&c_j;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>
</art>
