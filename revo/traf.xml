  <trd lng="ka">&#x10DB;&#x10DD;&#x10EE;&#x10D5;&#x10D3;&#x10D0;</trd>
  <trd lng="km">&#x1794;&#x17BB;&#x1780;</trd>
  <trd lng="ko">&#xD788;&#xD2B8;</trd>
  <trd lng="lo">&#xEA1;&#xEBB;&#xE99;&#xE95;&#xEB5;</trd>
  <trd lng="ml">&#xD24;&#xD32;&#xD4D;&#xD32;&#xD41;&#xD15;</trd>
  <trd lng="mr">&#x926;&#x93E;&#x92C;&#x93E;</trd>
  <trd lng="my">&#x1011;&#x102D;&#x102F;&#x1038;</trd>
  <trd lng="ne">&#x92E;&#x93E;&#x930;&#x93E;</trd>
  <trdgrp lng="nl">
    <trd>raken</trd>,
    <trd>treffen</trd>,
    <trd>halen<klr> (v.e.trein)</klr></trd>,
    <trd>terecht komen</trd>
  </trdgrp>
  <trd lng="ny">anagunda</trd>
