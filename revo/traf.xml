</drv>

<drv mrk="traf.0a">
  <kap><tld/>a</kap>
  <snc mrk="traf.0a.KOMUNE">
    <dif>
      <tld lit="T"/>anta, &gcirc;uste atinganta la celon:
      <ekz>
        <tld/>a respondo, argumento, rezulto, rimedo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>p&aacute;dn&yacute;</trd>,
    <trd>p&rcaron;il&eacute;hav&yacute;</trd>,
    <trd>trefn&yacute;</trd>
  </trdgrp>
  <trd lng="de">treffend</trd>
  <trdgrp lng="es">
    <trd>acertado</trd>,
    <trd>acertada</trd>,
    <trd>atinado</trd>,
    <trd>atinada</trd>
  </trdgrp>
  <trd lng="fr">qui fait <ind>mouche</ind></trd>
  <trd lng="hu">tal&aacute;l&oacute;</trd>
  <trd lng="nl">treffend</trd>
  <trdgrp lng="pl">
    <trd>trafny</trd>,
    <trd>celny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_t;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_u;&c_d;&c_a;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>priliehav&yacute;</trd>,
    <trd>p&aacute;dny</trd>,
    <trd>v&yacute;sti&zcaron;n&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="traf.0e">
  <kap><tld/>e</kap>
  <snc mrk="traf.0e.KOMUNE">
    <dif>
      En tute ta&ubreve;ga kaj alcela maniero:
      <ekz>
        <tld/>e ju&gcirc;i aferon;
      </ekz>
      <ekz>
        <tld/>e kopii ian manieron de parolado kaj irado<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>p&aacute;dn&ecaron;</trd>,
    <trd>p&rcaron;&iacute;padn&ecaron;</trd>,
    <trd>trefn&ecaron;</trd>,
    <trd>vhodn&ecaron;</trd>,
    <trd>v&yacute;sti&zcaron;n&ecaron;</trd>
  </trdgrp>
  <trd lng="de">treffend</trd>
  <trd lng="hu">tal&aacute;l&oacute;an</trd>
  <trd lng="nl">juist</trd>
  <trdgrp lng="pl">
    <trd>trafnie</trd>,
    <trd>celnie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_t;&c_k;&c_o;</trd>,
    <trd>&c_u;&c_d;&c_a;&c_ch;&c_n;&c_o;</trd>,
    <trd>&c_t;&c_o;&c_ch;&c_n;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>priliehavo</trd>,
    <trd>vhodne</trd>,
    <trd>v&yacute;sti&zcaron;ne</trd>
  </trdgrp>
</drv>

<drv mrk="traf.0o">
  <kap><tld/>o</kap>
  <snc mrk="traf.0o.KOMUNE">
    <dif>
      Ago de io a&ubreve; iu trafanta:
      <ekz>
        &ccirc;io, kio venas al la homo el ekstere, pasas kaj &scirc;an&gcirc;i&gcirc;as &ccirc;irka&ubreve;
        li sub la influo de iuj miloj da fluoj kaj kompliki&gcirc;oj, kun kiuj
        iras kaj reciproke interligi&gcirc;as la sociaj rilatoj kaj aran&gcirc;oj,
        sub la ofte plej terura, &ccirc;ar malplej anta&ubreve;videbla kaj
        anta&ubreve;kalkulebla influo de la blinda <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">strike</trd>
</drv>

<drv mrk="traf.0emal0e">
  <kap><tld/>e-mal<tld/>e,
  <var><kap><tld/>prove</kap></var></kap>
  <snc mrk="traf.0emal0e.KOMUNE">
    <dif>
      La&ubreve;hazarde, la&ubreve;&scirc;ance, blinde je l' <tld/>o:
      <ekz>
        la kompaso ne funkcias kaj, <tld/>e mal<tld/>e, ni &scirc;anceli&gcirc;as 
        ser&ccirc;ante la vojon
        <fnt>
          <aut>L. Frechette, trad. V. Beaupr&eacute;</aut>,
          <vrk><url ref="http://wallydutemple.com/wp/2016/01/06/ce-la-sojlo-louis-frechette-1839-1908-estis-franclingva-autoro-kiu-specialigis-pri-folkloraj-verkoj-de-la-pasinta-jarcento/">
          &Ccirc;e la Sojlo</url></vrk>,
          <lok>2016</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">nam&aacute;tkou</trd>
  <trd lng="fr">au petit bonheur la chance</trd>
  <trd lng="hu">tal&aacute;lomra</trd>
  <trd lng="nl">lukraak</trd>
  <trd lng="pl">na <ind>chybi&lstroke;-trafi&lstroke;</ind></trd>
  <trd lng="ru">&c_n;&c_a;&c_u;&c_g;&c_a;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>n&aacute;hodne</trd>,
    <trd>n&aacute;matkou</trd>
  </trdgrp>
</drv>

<drv mrk="traf.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="traf.0eco.KOMUNE">
    <dif>
      Akurateco de tio, kio <tld/>as la &gcirc;ustan lokon:
      <ekz>
        aliaj, frapitaj per la <tld/>eco de lia rimarko, komencis 
        rigardi lin kun miro
        <fnt><aut>H. Sienkiewicz, trad. L. Zamenhof</aut>, <vrk>Quo 
        vadis?</vrk>, <lok>1933</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">justesse</trd>
</drv>

<drv mrk="traf.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="traf.mal0i.KOMUNE">
    <dif>
      Kontra&ubreve;vole ne atingi la celita&jcirc;on:
      <ekz>
        kiu rigardas &ccirc;ielon, mal<tld/>as sian celon
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rev">disrevi&gcirc;i</ref>,
      <ref cel="fiask.0i">fiaski</ref>,
      <ref cel="sukces.mal0i">malsukcesi</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="lepor.0o">leporo</ref>,
      <ref cel="sxton.0o">&scirc;tono</ref>,
      <ref cel="naz.0o">nazo</ref>,
      <ref cel="har">lipharo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>chybit <klr>(c&iacute;l)</klr></trd>,
    <trd>chybovat</trd>,
    <trd>minout</trd>,
    <trd>m&iacute;jet</trd>,
    <trd>nezastihnout</trd>,
    <trd>prome&scaron;kat</trd>,
    <trd>prop&aacute;st</trd>,
    <trd>zme&scaron;kat</trd>
  </trdgrp>
  <trd lng="de">verpassen</trd>
  <trd lng="es">errar</trd>
  <trdgrp lng="fr">
    <trd>&eacute;chouer</trd>,
    <trd>rater</trd>,
    <trd>manquer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>elv&eacute;t</trd>,
    <trd>mell&eacute;tal&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">missen</trd>
  <trdgrp lng="pl">
    <trd>chybi&cacute;</trd>,
    <trd>spud&lstroke;owa&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_m;&c_a;&c_z;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>chybi&tcaron;</trd>,
    <trd>netrafi&tcaron;</trd>,
    <trd>nezasiahnu&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="traf.0povo">
  <kap><tld/>povo</kap>
  <snc>
    <dif>
      Distanco &gcirc;is kiu povas atingi &jcirc;etilo a&ubreve; ia ajn
      pafilo.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>dost&rcaron;el</trd>,
    <trd>mo&zcaron;nost dostihnut&iacute;</trd>,
    <trd>mo&zcaron;nost z&aacute;sahu</trd>
  </trdgrp>
  <trd lng="de">Reichweite</trd>
  <trd lng="es">capacidad de <ind>alcance</ind></trd>
  <trd lng="fr">port&eacute;e</trd>
  <trdgrp lng="hu">
    <trd>hat&oacute;t&aacute;v</trd>,
    <trd>hat&oacute;sug&aacute;r</trd>
  </trdgrp>
  <trd lng="nl">reikwijdte</trd>
  <trdgrp lng="pl">
