<drv mrk="traf.0a">
  <kap><tld/>a</kap>
  <snc mrk="traf.0a.KOMUNE">
    <dif>
      <tld lit="T"/>anta, &gcirc;uste atinganta la celon:
      <ekz>
        kiel <tld/>aj ofte estas liaj respondoj
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        per <tld/>aj &scirc;ercoj ili ofte elvokis &ccirc;iuflanke koran ridadon
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        post &ccirc;iu <tld/>a pafo &scirc;i <klr>[montras&#8230;]</klr> triumfantan rigardon
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la rimarko &scirc;ajnis tiel <tld/>a, ke li ekrigardis
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam lia kolero atingas sian kulminon, tiam fari&gcirc;as plej <tld/>aj <klr>[&#8230;]</klr>
        liaj ju&gcirc;kapablo, argumentado kaj batalrimedoj
        <fnt>
          <bib>Lanti</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="gxust.0a.celo">&gcirc;usta<sncref/></ref>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_a;&c_p;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>p&aacute;dn&yacute;</trd>,
    <trd>p&rcaron;il&eacute;hav&yacute;</trd>,
    <trd>trefn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>treffend</trd>,
    <trd>zutreffend</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>acertado</trd>,
    <trd>acertada</trd>,
    <trd>atinado</trd>,
    <trd>atinada</trd>
  </trdgrp>
  <trd lng="fr">qui fait <ind>mouche</ind></trd>
  <trd lng="hu">tal&aacute;l&oacute;</trd>
  <trd lng="nl">treffend</trd>
  <trdgrp lng="pl">
    <trd>trafny</trd>,
    <trd>celny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_t;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_u;&c_d;&c_a;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>priliehav&yacute;</trd>,
    <trd>p&aacute;dny</trd>,
    <trd>v&yacute;sti&zcaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&#x43B;&c_u;&c_ch;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_d;&c_a;&#x43B;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="traf.0e">
  <kap><tld/>e</kap>
  <snc mrk="traf.0e.KOMUNE">
    <dif>
      En tute ta&ubreve;ga kaj alcela maniero:
      <ekz>
        <tld/>e &jcirc;eti sagojn
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> malvarme kaj <tld/>e ju&gcirc;as la aferon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ion strangan kaj malbelan <nom>Kay</nom> povosciis kopii tre <tld/>e
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="akurat.0e">akurate</ref>,
       <ref cel="preciz.0e">precize</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_a;&c_p;&c_n;&c_a;</trd>
  <trdgrp lng="cs">
