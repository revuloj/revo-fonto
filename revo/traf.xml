<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: traf.xml,v 1.94 2022/05/05 17:08:11 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>traf</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="traf.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="traf.0i.atingi">
    <uzo tip="klr">(iu, io konkreta)</uzo>
    <dif>
      <ref tip="super" cel="ating.0i.tusxi">Atingi</ref> per &jcirc;etita 
      objekto a&ubreve; per sia propra movi&gcirc;anta korpo ion
      celitan; <ref tip="vid" cel="tusx.0i">tu&scirc;i</ref>:
      <ekz>
        li prenis &scirc;tonon kaj <tld/>is la birdon, kiu falis teren
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bovo <klr>[&#8230;]</klr> saltis anta&ubreve;en, <tld/>is la lanciston
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        en mia vagado mi <tld/>is la mona&hcirc;inejon 
        pli-malpli &ccirc;e la vesperi&gcirc;o
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> &ccirc;irka&ubreve;irante la disputantojn, 
        ree <tld/>is la vojon en loko, kie
        ili ne povis &scirc;in vidi
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        vidas okulo, sed mano ne <tld/>as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind><tld/>i du celojn per unu &scirc;tono</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">k&eacute;t legyet &uuml;t egy csap&aacute;sra</trd>
        <trd lng="pt">acertar</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i per la parolo rekte en la viza&gcirc;on
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="rendev.0i">rendevui</ref>,
       <ref cel="renkon.0i.trafi">renkonti<sncref/></ref>
    </refgrp>
    <trdgrp lng="es">
      <trd>atinar</trd>,
      <trd>alcanzar</trd>,
      <trd>acertar</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>atteindre</trd>,
      <trd>rencontrer</trd>,
      <trd>toucher<klr> (un but)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>tal&aacute;l</trd>,
      <trd>eltal&aacute;l</trd>
    </trdgrp>
    <trd lng="pl">trafia&cacute;</trd>
    <trd lng="ru">&c_p;&c_o;&c_p;&c_a;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="traf.0i.frapi">
    <uzo tip="stl">FIG</uzo>
    <uzo tip="klr">(io abstrakta)</uzo>
    <dif>
      Atingi,
      <ref tip="vid" cel="frap.0i">frapi</ref>:
      <ekz>
        la mateno <tld/>is ilin en <nom>&Hcirc;ebron</nom>
        <fnt>
          <bib>MT</bib>
          <lok>Ii. samuel 2:32</lok>
        </fnt>;
      </ekz>
      <ekz>
        la loto <tld/>is Saulon
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 10:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvieta morto &scirc;in <tld/>is
        <fnt>
          <bib>Fab2</bib>
          <lok>Elfo de la rozo</lok>
        </fnt>;
      </ekz>
      <ekz>
        tondro <tld/>is mian dormetantan orelon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne hontis konfesi la trompi&gcirc;ojn kiuj <klr>[&#8230;]</klr> min <tld/>is
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        malfeli&ccirc;o <klr>[&#8230;]</klr>  <tld/>is lin
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 2:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        stranga okazo min <tld/>is
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj okuloj tute mirigite <tld/>is la pentritan bildon de la patrino
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        neniu scias, kio morga&ubreve; lin <tld/>os
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
       <ref cel="kapt.0i.surprizi">kapti<sncref/></ref>,
       <ref cel="surpri.0i.alveni">surprizi<sncref/></ref>
    </refgrp>
    <trdgrp lng="es">
      <trd>alcanzar</trd>,
      <trd>encontrar</trd>
    </trdgrp>
    <trd lng="hu">&eacute;r <klr tip="ind">(hat r&aacute;)</klr></trd>
    <trdgrp lng="pl">
      <trd>przyda&zdot;a&cacute;<klr> si&eogonek;</klr></trd>,
      <trd>spotyka&cacute;<klr> (co&sacute; - kogo&sacute;)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_r;&c_a;&c_z;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_n;&c_a;&c_s;&c_t;&c_i;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="traf.0i.renkonti">
    <dif>
      <ref tip="sin" cel="renkon.0i.trafi">Renkonti</ref> ian konkretan baron; 
      <ref tip="sin" cel="fal.0i.en_ion">fali<sncref/></ref>:
      <ekz>
        la veturilo <klr>[&#8230;]</klr> <tld/>is en kavon
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is tripiedon kaj vundis brovon
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj el tiuj libroj <klr>[&#8230;]</klr> <tld/>is anka&ubreve; en la manojn de la
        imperiestro
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>is fal&ccirc;ilo sur &scirc;tonon</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">belet&ouml;rt a bicsk&aacute;ja</trd>
        <trd lng="pl">trafi&lstroke;a kosa na kamie&nacute;</trd>
        <trd lng="ru">&c_n;&c_a;&c_sh;&c_l;&c_a; &c_k;&c_o;&c_s;&c_a; &c_n;&c_a; &c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;</trd>
      </ekz>
      <ekz>
        <tld/>i per la viza&gcirc;o en koton
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>li <tld/>is el sub pluvo en riveron</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
        <trd lng="de">er kam vom <ind>Regen</ind> in die Traufe</trd>
        <trd lng="pl">trafi&cacute; z deszczu pod rynn&eogonek;</trd>
      </ekz>
      <ekz>
        celi anseron, <tld/>i aeron
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="kolizi.0i">kolizii</ref>
    </refgrp>
    <trdgrp lng="es">
      <trd>tropezar</trd>,
      <trd>caer</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>ker&uuml;l <klr tip="ind">(vhova)</klr></trd>,
      <trd>jut <klr tip="ind">(vhova)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>trafia&cacute;<klr> na/w co&sacute;</klr></trd>,
      <trd>natrafia&cacute;<klr> na co&sacute;</klr></trd>,
      <trd>natyka&cacute; si&eogonek;<klr> na co&sacute;</klr></trd>,
      <trd>natkn&aogonek;&cacute; si&eogonek;<klr> na
      co&sacute;</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_p;&c_a;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_n;&c_a;&c_t;&c_k;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc mrk="traf.0i.sperti">
    <uzo tip="stl">FIG</uzo>
    <uzo tip="klr">(iu, io)</uzo>
    <dif>
      <ref cel="spert.0i">Sperti</ref>, <ref tip="super" cel="renkon.0i.trafi">renkonti</ref>
      ion abstraktan:
      <ekz>
        povus <tld/>i min malfeli&ccirc;o kaj mi mortus
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 19:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        por <tld/>i tian rezultaton, oni suferas tiom da moralaj kaj fizikaj doloroj
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        tian taskon ne tiel <tld/>e kaj sukcese povas plenumi organizoj samcelaj
        <fnt>
          <bib>Lanti</bib>
        </fnt>;
      </ekz>
      <ekz>
        la verko de la filo trafis pli favoran akcepton kaj estis plurfoje represita
        <fnt>
          <bib>Monato</bib>
          <aut>Andreo Lovaniensis</aut>
          <vrk><url ref="&Monato;/2011/010174.php">Surpriza proverbaro</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>kap</trd>,
      <trd>r&eacute;szes&uuml;l</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>trafia&cacute; si&eogonek;</trd>,
      <trd>przytrafia&cacute; si&eogonek;</trd>,
      <trd>spotyka&cacute; si&eogonek;<klr> (z
      czym&sacute;)</klr></trd>,
      <trd>doznawa&cacute;</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_o;&c_p;&c_a;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="af">getref</trd>
  <trd lng="am">&#x12ED;&#x121D;&#4721;</trd>
  <trd lng="ar">&a_D;&a_r;&a_b;</trd>
  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_a;&c_p;&c_l;&c_ja;&c_c;&c_mol;</trd>,
