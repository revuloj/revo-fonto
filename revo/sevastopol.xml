<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: sevastopol.xml,v 1.3 2023/05/06 14:26:04 afido Exp $">
    <kap>
      <rad>Sevastopol</rad>/o
    </kap>

<drv mrk="sevastopol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      &Ccirc;efa <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      de <ref tip="malprt" cel="krime.0o">Krimeo</ref>, &ccirc;e la sudokcidenta bordo:
      <ekz>
        en <tld/>o ni restis du tagojn, vidis la vidinda&jcirc;ojn en kaj &ccirc;irka&ubreve; la urbo kaj
        forveturis Jalton en po&scirc;ta veturilo
        <fnt>
          <aut>Valdemar Langlet</aut>
          <vrk><url ref="&Tekstaro;?nomo=vojaghimpresoj">Voja&gcirc;impresoj</url></vrk>
          <lok>Impresoj de svedoj, voja&gcirc;antaj en Rusujo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la antikva urbo &Hcirc;ersoneso kies ruinoj trovi&gcirc;as apud <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=363590">2701 Cherson</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_S;&c_je;&c_v;&c_a;&c_s;&c_t;&c_o;&c_p;&c_a;&c_l;&c_mol;</trd>
    <trd lng="de">Sewastopol</trd>
    <trd lng="en">Sevastopol</trd>
    <trd lng="fr">S&eacute;bastopol</trd>
    <trd lng="grc">&Sigma;&epsilon;&beta;&alfa;&sigma;&tau;&omikron;&ypsilon_ton;&pi;&omikron;&lambda;&jota;&sigma_fina;</trd>
    <trd lng="ru">&c_S;&c_je;&c_v;&c_a;&c_s;&c_t;&c_o;&c_p;&c_o;&c_l;&c_mol;</trd>
    <trd lng="uk">&c_S;&c_je;&c_v;&c_a;&c_s;&c_t;&c_o;&c_p;&c_o;&c_l;&c_mol;</trd>
  </snc>
</drv>

  </art>

