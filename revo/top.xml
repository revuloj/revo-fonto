<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>top</rad>/o
</kap>
<drv mrk="top.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Elstara plato &ccirc;e la supro de la unua parto de la masto.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Mastkorb</trd>,
    <trd>Mars</trd>
  </trdgrp>
  <trd lng="en">crow's nest</trd>
  <trd lng="nl">mars<klr tip="ind"> (v.schip)</klr></trd>
  <trd lng="ru">&c_m;&c_a;&c_r;&c_s; <klr tip="amb">(&c_n;&c_a; &c_m;&c_a;&c_ch;&c_t;&c_je;)</klr></trd>
</drv>

<drv mrk="top.0masto">
  <kap><tld/>masto</kap>
  <snc>
    <dif>
      Masto, kiu estas super la <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Marsstenge</trd>
  <trd lng="en">top mast</trd>
  <trd lng="nl">marssteng</trd>
  <trd lng="ru">&c_t;&c_o;&c_p;-&c_m;&c_a;&c_ch;&c_t;&c_a;</trd>
</drv>

<drv mrk="top.0velo">
  <kap><tld/>velo</kap>
  <snc>
    <dif>
      Velo de <tld/>masto.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Toppsegel</trd>,
    <trd>Marssegel</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>top sail</trd>,
    <trd>tops'l</trd>
  </trdgrp>
  <trd lng="nl">marszeil</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_p;&c_s;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_h;&c_n;&c_i;&c_j; <ind>&c_p;&c_a;&c_r;&c_u;&c_s;</ind></trd>
  </trdgrp>
</drv>
</art>
