<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>top</rad>/o
</kap>
<drv mrk="top.0o">
  <kap><tld/>o</kap>
  <snc mrk="top.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Elstara plato &ccirc;e la supro de la unua parto de <ref tip="malprt"
      cel="mast.0o.MAR">masto</ref>:
      <ekz>
        mi alligis tri hokegojn al la <tld/>oj de la tri mastoj
        <fnt><bib>Munchhausen</bib>, <lok>&ccirc;apitro 16a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">cofa</trd>
  <trdgrp lng="cs">
    <trd>stěžňová plošina</trd>,
    <trd>stěžňový koš</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Mastkorb</trd>,
    <trd>Mars</trd>
  </trdgrp>
  <trd lng="en">crow's nest</trd>
  <trdgrp lng="hu">
    <trd>&aacute;rbockos&aacute;r</trd>,
    <trd>tereb</trd>
  </trdgrp>
  <trd lng="nl">mars<klr tip="ind"> (v.schip)</klr></trd>
  <trdgrp lng="pt">
    <trd>cesto da g&aacute;vea</trd>,
    <trd>g&aacute;vea</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_r;&c_s; <klr tip="amb">(&c_n;&c_a; 
&c_m;&c_a;&c_ch;&c_t;&c_je;)</klr></trd>,
    <trd>&c_t;&c_o;&c_p;</trd>,
    <trd>&c_v;&c_o;&c_r;&c_o;&c_n;&c_mol;&c_je; 
&c_g;&c_n;&c_je;&c_z;&c_d;&c_o;<klr tip="ind"> (&c_n;&c_a; 
&c_m;&c_a;&c_ch;&c_t;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="sk">sťažňová plošinka</trd>
</drv>

</art>
