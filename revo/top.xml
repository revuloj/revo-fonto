<art mrk="$Id: top.xml,v 1.20 2024/01/18 21:22:58 revo Exp $">
<kap>
  <rad>top</rad>/o
</kap>
<drv mrk="top.0o">
  <kap><tld/>o</kap>
  <snc mrk="top.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Elstara plato &ccirc;e la supro de la unua parto de <ref tip="malprt" cel="mast.0o.MAR">masto</ref>:
      <ekz>
        mi alligis tri hokegojn al la <tld/>oj de la tri mastoj
        <fnt><bib>Munchhausen</bib>, <lok>&ccirc;apitro 16a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">cofa</trd>
  <trdgrp lng="cs">
    <trd>st&ecaron;&zcaron;&ncaron;ov&aacute; plo&scaron;ina</trd>,
    <trd>st&ecaron;&zcaron;&ncaron;ov&yacute; ko&scaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Mastkorb</trd>,
    <trd>Mars</trd>
  </trdgrp>
  <trd lng="en">crow's nest</trd>
  <trd lng="es">cofa<klr> (puesto de vig&iacute;a desde el m&aacute;stil)</klr></trd>
  <trdgrp lng="hu">
    <trd>&aacute;rbockos&aacute;r</trd>,
    <trd>tereb</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30C8;&#x30C3;&#x30D7;  <pr>&#12392;&#12387;&#12407;</pr></trd>,
    <trd>&#x6AA3;&#x697C;  <pr>&#12375;&#12423;&#12358;&#x308D;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">mars<klr tip="ind"> (v.schip)</klr></trd>
  <trd lng="pl">top</trd>
  <trdgrp lng="pt">
    <trd>cesto da g&aacute;vea</trd>,
    <trd>g&aacute;vea</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_r;&c_s; <klr tip="amb">(&c_n;&c_a;
&c_m;&c_a;&c_ch;&c_t;&c_je;)</klr></trd>,
    <trd>&c_t;&c_o;&c_p;</trd>,
    <trd>&c_v;&c_o;&c_r;&c_o;&c_n;&c_mol;&c_je;
&c_g;&c_n;&c_je;&c_z;&c_d;&c_o;<klr tip="ind"> (&c_n;&c_a;
&c_m;&c_a;&c_ch;&c_t;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="sk">s&tcaron;a&zcaron;&ncaron;ov&aacute; plo&scaron;inka</trd>
  <trd lng="uk">&#x43C;&c_a;&c_r;&c_s;</trd>
</drv>

</art>
