<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>top</rad>/o
</kap>
<drv mrk="top.0o">
<kap><tld/>o</kap>
<uzo tip="fak">MAR</uzo>
<snc>
<dif>
Elstara plato &ccirc;e la supro de la unua parto de la masto. 
</dif>
</snc>
<trdgrp lng="de">
   <trd>Mastkorb</trd>,
   <trd>Mars</trd>
</trdgrp>
</drv>
<drv mrk="top.0masto">
<kap><tld/>masto</kap>
<snc>
<dif>
Masto, kiu estas super la <tld/>o. 
</dif>
</snc>
<trd lng="de">Marsstenge</trd>
</drv>
<drv mrk="top.0velo">
<kap><tld/>velo</kap>
<snc>
<dif>
Velo de <tld/>masto.  
</dif>
</snc>
<trdgrp lng="de">
   <trd>Toppsegel</trd>,
   <trd>Marssegel</trd>
</trdgrp>
</drv>
</art>
<!--
$Log$
-->
</vortaro>

