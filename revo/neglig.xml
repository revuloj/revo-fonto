<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>negli&gcirc;</rad>/o
</kap>
<drv mrk="neglig.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="neglig.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Hejma senceremonia vesto:
      <ekz>
        &scirc;i portis blankan <tld/>on, apena&ubreve; al&gcirc;ustigitan &ccirc;e la talio
        <fnt>
        <aut>Machado de Assis, trad. Paulo Viana</aut>,
        <vrk>La divenistino kaj aliaj rakontoj</vrk>,
        <lok>Oportuno, jaro 2005a, p. 29a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_e;&c_g;&c_l;&c_ib;&c_zh;&c_e;</trd>
  <trdgrp lng="de">
    <trd>Morgenkleid</trd>,
    <trd>Morgenrock</trd>,
    <trd>Schlafrock</trd>,
    <trd>Negligee</trd>
  </trdgrp>
  <trd lng="en">housedress</trd>
  <trdgrp lng="fr">
    <trd>n&eacute;glig&eacute;<klr tip="amb"> (v&ecirc;tement d'int&eacute;rieur)</klr></trd>,
    <trd>v&ecirc;tement d'int&eacute;rieur</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>pongyola</trd>,
    <trd>h&aacute;ziruha</trd>,
    <trd>neglizs&eacute;</trd>
  </trdgrp>
  <trd lng="nl">neglig&eacute;</trd>
  <trdgrp lng="pt">
    <trd>roup&atilde;o</trd>,
    <trd>chambre</trd>,
    <trd>n&eacute;glig&eacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_g;&c_l;&c_i;&c_zh;&c_je;</trd>,
    <trd>&c_d;&c_je;&c_z;&c_a;&c_b;&c_i;&c_l;&c_mol;&c_je;</trd>
  </trdgrp>
</drv>
</art>
