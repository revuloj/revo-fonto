<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tenis.xml,v 1.37 2023/10/21 13:02:26 revo Exp $">
<kap>
  <ofc>9</ofc><rad>tenis</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="tenis.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="tenis.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="super" cel="sport.0o">Sportludo</ref>, en kiu du
      a&ubreve; kvar ludantoj per specialaj batiloj
      (<ref tip="prt" cel="raked.0o.SPO">rakedoj</ref>)
      alterne sendas pilketon trans mezan reton al la
      kontra&ubreve;a parto de la ludejo:
      <ekz>
        li estas ludanta <tld/>on
        <fnt>
          <aut>trad. ciifrancesco</aut>,
          <vrk><url ref="http://www.opensubtitles.org/fr/subtitles/4010951/il-bisbetico-domato-eo">
          La Dresado de la Grumblemulo</url></vrk>,
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_n;&c_ib;&c_s;</trd>
  <trd lng="bg">&c_t;&c_je;&c_n;&c_i;&c_s;</trd>
  <trd lng="ca">tennis</trd>
  <trd lng="de">Tennis</trd>
  <trd lng="en">tennis</trd>
  <trd lng="es">tenis</trd>
  <trd lng="fr">tennis</trd>
  <trd lng="hu">tenisz</trd>  
  <trdgrp lng="ja">
    <trd>&#x30C6;&#x30CB;&#x30B9;  <pr>&#x3066;&#x306B;&#x3059;</pr></trd>,
    <trd>&#x5EAD;&#x7403;  <pr>&#x3066;&#x3044;&#x304D;&#x3085;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">tennis</trd>
  <trd lng="pl">tenis</trd>
  <trdgrp lng="pt">
    <trd>t&ecirc;nis</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_n;&c_n;&c_i;&c_s;</trd>
  <trd lng="sk">tenis</trd>
  <trd lng="sv">tennis</trd>
  <trd lng="uk">&c_t;&c_je;&#x43D;&c_ib;&c_s;</trd>
</drv>

<drv mrk="tenis.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="tenis.0ejo.areno">
    <dif>
      Ortograma areo (proksimume 11&#xD7;24m granda), kun
      dismarkitaj zonoj kaj reto, aran&gcirc;ita por ludi <tld/>on:
      <ekz>
        dum tenisaj turniroj gravas anta&ubreve; &ccirc;io
        forto de frapo, sed la veran monon oni atingas ne en
        tenisejo
        <fnt>
          <aut>S. &Sacute;migielski</aut>:
          <vrk>Belulinoj en tenisejoj</vrk>,
          <bib>Monato</bib>,
          <lok>2002/02, p. 21</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_n;&c_ib;&c_s;&c_n;&c_y;
  &c_k;&c_o;&c_r;&c_t;</trd>
  <trd lng="bg">&c_t;&c_je;&c_n;&c_i;&c_s;&c_k;&c_o;&c_r;&c_t;</trd>
  <trd lng="ca">pista de tennis</trd>
  <trdgrp lng="cs">
    <trd>kurt</trd>,
    <trd>tenisov&eacute; h&rcaron;i&scaron;t&ecaron;</trd>,
    <trd>tenisov&yacute; dvorec</trd>
  </trdgrp>
  <trd lng="de">Tennisplatz</trd>
  <trd lng="en">tennis court</trd>
  <trd lng="es">cancha de tenis</trd>
  <trd lng="fr">court de tennis</trd>
  <trd lng="hu">teniszp&aacute;lya</trd>
  <trd lng="ja">&#x30C6;&#x30CB;&#x30B9;&#x30B3;&#x30FC;&#x30C8;</trd>
  <trd lng="nl">tennisbaan</trd>
  <trd lng="pl">kort tenisowy</trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>kurt</trd>,
    <trd>tenisov&eacute; ihrisko</trd>,
    <trd>tenisov&yacute; dvorec</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>tennisbana</trd>,
    <trd>tennisplan</trd>
  </trdgrp>
  <trd lng="uk">&c_t;&c_je;&#x43D;&c_ib;&c_s;&#x43D;&c_i;&c_j; &#x43A;&#x43E;&c_r;&c_t;</trd>
</drv>

<drv mrk="tenis.tablo0o">
  <kap><ofc>9</ofc>tablo<tld/>o <fnt><bib>PIV1</bib></fnt></kap>
  <snc mrk="tenis.tablo0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="super" cel="sport.0o">Sportludo</ref> simila al
      <tld/>o, ludata sur speciala tablo per malgrandaj rigidaj
      rakedoj:
      <ekz>
        post la intervjuo li ankora&ubreve; ludos partion de
        tablo<tld/>o
        <fnt>
        <aut>Anne Tilmont</aut> <vrk><url ref="&Monato;/1998/005932.php">Kosmopolita civitano kun okulo al la universo</url></vrk>
        <bib>Monato</bib>
        <lok>1998/04, p. 9</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_s;&c_t;&c_o;&c_l;&c_mol;&c_n;&c_y;
  <ind>&c_t;&c_e;&c_n;&c_ib;&c_s;</ind></trd>
  <trdgrp lng="bg">
    <trd>&c_t;&c_je;&c_n;&c_i;&c_s; &c_n;&c_a;
    &c_m;&c_a;&c_s;&c_a;</trd>,
    <trd>&c_p;&c_i;&c_n;&c_g;-&c_p;&c_o;&c_n;&c_g;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>ping-pong</trd>,
    <trd>tennis de taula</trd>
  </trdgrp>
  <trd lng="de">Tischtennis</trd>
  <trdgrp lng="en">
    <trd>ping pong</trd>,
    <trd>table tennis</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>tenis de mesa</trd>,
    <trd>ping pong</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>pingpong</trd>,
    <trd>tennis de table</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>asztalitenisz</trd>,
    <trd>pingpong</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5353;&#x7403;  <pr>&#x305F;&#x3063;&#x304D;&#x3085;&#x3046;</pr></trd>,
    <trd>&#x30D4;&#x30F3;&#x30DD;&#x30F3;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>tafeltennis</trd>,
    <trd>pingpong</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>t&ecirc;nis de mesa</trd>,
    <trd>pingue-pongue</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ping-pong</trd>,
    <trd>stoln&yacute; tenis</trd>
  </trdgrp>
  <trd lng="pl">tenis sto&lstroke;owy</trd>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_t;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j;
  &c_t;&c_je;&c_n;&c_n;&c_i;&c_s;</trd>
  <trdgrp lng="sv">
    <trd>bordtennis</trd>,
    <trd>pingis</trd>,
    <trd>pingpong</trd>
  </trdgrp>
</drv>
</art>
