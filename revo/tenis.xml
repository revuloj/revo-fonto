<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>tenis</rad>/o
    </kap>

    <drv mrk="tenis.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">SPO</uzo>
      <snc>
        <dif>
          <ref tip="super" cel="sport.0o">Sportludo</ref>, en kiu du
          a&ubreve; kvar ludantoj per specialaj batiloj
          (<ref tip="prt" cel="raked.0o">rakedoj</ref>)
          alterne &jcirc;etas kaj re&jcirc;etas pilketon trans mezan reton al la
          kontra&ubreve;a parto de la ludejo.
        </dif>
      </snc>
      <trd lng="be">&c_t;&c_e;&c_n;&c_ib;&c_s;</trd>
      <trd lng="de">Tennis</trd>
      <trd lng="en">tennis</trd>
      <trd lng="fr">tennis</trd>
      <trd lng="hu">tenisz</trd>  
      <trd lng="nl">tennis</trd>
      <trd lng="pl">tenis</trd>
      <trdgrp lng="pt">
        <trd>t&ecirc;nis</trd>
      </trdgrp>
      <trd lng="ru">&c_t;&c_je;&c_n;&c_n;&c_i;&c_s;</trd>
      <trd lng="sk">tenis</trd>
    </drv>

    <drv mrk="tenis.0ejo">
      <kap><tld/>ejo</kap>
      <snc mrk="tenis.0ejo.areno">
        <dif>
          Ortograma areo (proksimume 40&#xd7;20m granda), kun
          dismarkitaj zonoj kaj reto, aran&gcirc;ita por ludi <tld/>on:
          <ekz>
            dum tenisaj turniroj gravas anta&ubreve; &ccirc;io
            forto de frapo, sed la veran monon oni atingas ne en
            tenisejo
            <fnt>
              <aut>Stanis&lstroke;aw &Sacute;migielski</aut>:
              <vrk>Belulinoj en tenisejoj</vrk>,
              <bib>Monato</bib><lok>jaro 2002a, numero 2a, p. 21a</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_t;&c_e;&c_n;&c_ib;&c_s;&c_n;&c_y; &c_k;&c_o;&c_r;&c_t;</trd>
      <trdgrp lng="en">
        <trd>tennis court</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>court de tennis</trd>
      </trdgrp>
      <trd lng="hu">teniszp&aacute;lya</trd>
      <trd lng="pl">kort tenisowy</trd>  
      <trd lng="ru">&c_k;&c_o;&c_r;&c_t;</trd>
    </drv>

    <drv mrk="tenis.tablo0o">
      <kap>tablo<tld/>o</kap>
      <uzo tip="fak">SPO</uzo>
      <snc>
        <dif>
          <ref tip="super" cel="sport.0ludo">Sportludo</ref> simila al
          <tld/>o, ludata sur speciala tablo per malgrandaj rigidaj
          rakedoj:
          <ekz>
            post la intervjuo li ankora&ubreve; ludos partion de tablo<tld/>o
            <fnt><aut>Anne Tilmont</aut>:
              <vrk>Arthur C. Clarke &ndash;
                kosmopolita civitano kun okulo al la universo</vrk>,
              <bib>Monato</bib><lok>jaro 1998a, numero 4a, p. 9a</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_n;&c_a;&c_s;&c_t;&c_o;&c_l;&c_mol;&c_n;&c_y;
<ind>&c_t;&c_e;&c_n;&c_ib;&c_s;</ind></trd>
      <trdgrp lng="en">
        <trd>ping pong</trd>,
        <trd>table tennis</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>asztalitenisz</trd>,
        <trd>pingpong</trd>
      </trdgrp>
      <trdgrp lng="pt">
        <trd>t&ecirc;nis de mesa</trd>,
        <trd>pingue-pongue</trd>
      </trdgrp>
      <trdgrp lng="sk">
        <trd>ping-pong</trd>,
        <trd>stoln&yacute; tenis</trd>
      </trdgrp>
      <trd lng="pl">tenis sto&lstroke;owy</trd>
      <trd lng="ru">&c_n;&c_a;&c_s;&c_t;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j;
&c_t;&c_je;&c_n;&c_n;&c_i;&c_s;</trd>
    </drv>
  </art>
