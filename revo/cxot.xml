<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&ccirc;ot</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="cxot.0o">
  <kap><tld/>o</kap>
  <snc mrk="cxot.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="fisx.0o" lst="voko:riverfi&scirc;oj">Fi&scirc;o</ref> 
      kun piramidforma kapo, vivanta en riveroj;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="cxoted.0oj"><tld/>edoj</ref>
      (<trd lng="la">Cottus</trd>).
    </dif>
  </snc>
  <trd lng="ca">g&egrave;nere Cottus</trd>
  <trdgrp lng="de">
    <trd>Groppe</trd>,
    <trd>Koppe</trd>
  </trdgrp>
  <trd lng="fr">chabot</trd>
  <trd lng="hu">k&ouml;l&ouml;nte</trd>
  <trdgrp lng="pl">
    <trd>g&lstroke;owacz</trd>,
    <trd>baba<klr> (pot.)</klr></trd>,
    <trd>babka<klr> (pot.)</klr></trd>,
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_k;&c_a;&c_m;&c_je;&c_n;&c_shch;&c_i;&c_k;</trd>,
    <trd>&c_b;&c_y;&c_ch;&c_o;&c_k;-&c_r;&c_o;&c_g;&c_a;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_i;&c_r;&c_o;&c_k;&c_o;&c_l;&c_o;&c_b;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

</art>

