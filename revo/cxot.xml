<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>&ccirc;ot</rad>/o
</kap>
<drv mrk="cxot.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="cxot.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Cottus</trd>), fi&scirc;o kun piramidforma kapo.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Groppe</trd>,
    <trd>Koppe</trd>
  </trdgrp>
</drv>
<drv mrk="cxot.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo
      <ref tip="super" cel="skorpen.0oformaj">skorpenoformaj fi&scirc;oj</ref>
      (<trd lng="la">Cottidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Groppen</trd>
</drv>
</art>

