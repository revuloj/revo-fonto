<vortaro>
<art mrk="$Id$">
<kap>
<rad>&ccirc;ot</rad>/o
</kap>
<drv mrk="cxot.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="cxot.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Cottus</trd>), fi&scirc;o kun piramidforma kapo.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Groppe</trd>,
    <trd>Koppe</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_k;&c_a;&c_m;&c_je;&c_n;&c_shch;&c_i;&c_k;</trd>,
    <trd>&c_b;&c_y;&c_ch;&c_o;&c_k;-&c_r;&c_o;&c_g;&c_a;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_i;&c_r;&c_o;&c_k;&c_o;&c_l;&c_o;&c_b;&c_k;&c_a;</trd>    
  </trdgrp>
</drv>
<drv mrk="cxot.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo
      <ref tip="super" cel="skorpen.0oformaj">skorpenoformaj fi&scirc;oj</ref>
      (<trd lng="la">Cottidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Groppen</trd>
</drv>
</art>

