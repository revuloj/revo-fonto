<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cxot.xml,v 1.21 2023/10/31 13:37:04 revo Exp $">
<kap>
  <rad>&ccirc;ot</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="cxot.0o">
  <kap><tld/>o</kap>
  <snc mrk="cxot.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="fisx.0o" lst="voko:fi&scirc;oj">Fi&scirc;o</ref> 
      kun piramidforma kapo, vivanta en riveroj;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio <ref tip="malprt" cel="cxoted.0oj"><tld/>edoj</ref>
      (<trd lng="la">Cottus</trd>):
      <ekz>
        en marto <klr>[&#x2026;]</klr> frajas ezoko, &hcirc;ondrostomo kaj <tld/>o
        <fnt>
          <aut>Ev&zcaron;en Wohlgemuth</aut>
          <vrk><url ref="http://www.literatura.bucek.name/brno/wohlgemuth-evzen_printempo-venas.pdf">Printempo
          venas</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="&WCU;/thumb/2/28/Cottus_gobio_(in_situ).jpg/640px-Cottus_gobio_(in_situ).jpg" prm="CC   BY-SA 4.0"><tld/>o (Cottus gobio)
      <fnt>
        <aut>&#xA9; Hans Hillewaert, propra verko</aut>
        <lok><url ref="&WCW;/File:Cottus_gobio_(in_situ).jpg">&WCW;/File:Cottus_gobio_(in_situ).jpg</url></lok>
      </fnt>
  </bld>
  <trd lng="be">&c_b;&c_y;&c_ch;&c_o;&c_k; <klr>(&c_r;&c_y;&c_b;&c_a;)</klr></trd>
  <trd lng="ca">g&egrave;nere Cottus</trd>
  <trdgrp lng="de">
    <trd>Groppe</trd>,
    <trd>Koppe</trd>
  </trdgrp>
  <trd lng="fr">chabot</trd>
  <trd lng="he">&#x5E1;&#x5D5;&#x5D2; &#x5D3;&#x5D2;</trd>
  <trd lng="hu">k&ouml;l&ouml;nte</trd>
  <trd lng="ja">&#x30AB;&#x30B8;&#x30AB;</trd>
  <trdgrp lng="pl">
    <trd>g&lstroke;owacz</trd>,
    <trd>baba<klr> (pot.)</klr></trd>,
    <trd>babka<klr> (pot.)</klr></trd>,
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_k;&c_a;&c_m;&c_je;&c_n;&c_shch;&c_i;&c_k;</trd>,
    <trd>&c_b;&c_y;&c_ch;&c_o;&c_k;-&c_r;&c_o;&c_g;&c_a;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_i;&c_r;&c_o;&c_k;&c_o;&c_l;&c_o;&c_b;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x675C;&#x7236;&#x9B5A; <pr>d&#xF9;f&#xF9;y&#xFA;</pr></trd>,
    <trd>&#x9BF0;&#x9B5A; <pr>ni&#xE1;ny&#xFA;</pr></trd>
  </trdgrp>
</drv>

</art>

