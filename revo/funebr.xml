<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: funebr.xml,v 1.23 2023/08/31 13:11:32 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>funebr</rad>/o
</kap>
<drv mrk="funebr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="funebr.0o.sento">
    <dif>
      Mal&gcirc;oja sento pro ies morto, pro malfeli&ccirc;ego:
      <ekz>
        estas granda <tld/>o en la domo, en la tuta lando.
      </ekz>
    </dif>
    <trd lng="de">Trauer</trd>
    <trdgrp lng="en">
      <trd>mourning</trd>,
      <trd>grief</trd>
    </trdgrp>
    <trd lng="es">luto</trd> 
    <trdgrp lng="fa">
      <trd>&a_s;&a_w;&f_g;</trd>,
      <trd>&a_m;&a_A;&a_t;&a_m;</trd>,
      <trd>&a_ayn;&a_z;&a_A;</trd>
    </trdgrp>
    <trd lng="hu">gy&aacute;sz</trd>    
    <trd lng="nl">rouw</trd>
    <trdgrp lng="pt">
      <trd>luto</trd>,
      <trd>nojo</trd>
    </trdgrp>
  </snc>
  <snc mrk="funebr.0o.moro">
    <dif>
      Eksteraj montroj de tia mal&gcirc;ojo; <tld/>a ceremonio:
      <ekz>
        piuloj enterigis Stefanon, kaj faris pri li grandan <tld/>on
        <fnt><bib>NT</bib>, <lok>&Ago; 8:2</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>o da&ubreve;ris unu jaron<fnt>B</fnt>;
      </ekz>
      <ekz>
        multaj personoj &ccirc;eestis la <tld/>on.
      </ekz>
    </dif>
    <trd lng="de">Trauerfeier</trd>
    <trdgrp lng="en">
      <trd>mourning</trd>,
      <trd>grief</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>luto</trd>,
      <trd>funeral</trd>
    </trdgrp> 
    <trdgrp lng="fa">
      <trd>&a_m;&a_r;&a_A;&a_s;&a_m; <ind>&a_s;&a_w;&f_g;&a_w;&a_A;&a_r;&f_y;</ind></trd>,
      <trd>&a_m;&a_r;&a_A;&a_s;&a_m; <ind>&a_ayn;&a_z;&a_A;&a_d;&a_A;&a_r;&f_y;</ind></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>fun&eacute;railles</trd>,
      <trd>obs&egrave;ques</trd>,
    </trdgrp>
    <trd lng="hu">gy&aacute;szol&aacute;s</trd>    
    <trd lng="nl">rouw</trd>
  <trdgrp lng="pt">
    <trd>luto</trd>,
    <trd>nojo</trd>
  </trdgrp>
  </snc>
  <snc mrk="funebr.0o.vesto">
    <dif>
      Vestoj de <tld/>o:
      <ekz>
        survesti, porti <tld/>on pri iu<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">Trauerkleidung</trd>
    <trdgrp lng="en">
      <trd>mourning garments</trd>,
      <trd>mourning band</trd>,
      <trd>weeds<klr tip="amb"> (mourning clothes)</klr></trd>
    </trdgrp>
    <trd lng="es">luto</trd> 
    <trdgrp lng="fa">
      <trd>&a_l;&a_b;&a_A;&a_s; <ind>&a_s;&a_w;&f_g;&a_w;&a_A;&a_r;&f_y;</ind></trd>,
      <trd>&a_l;&a_b;&a_A;&a_s; <ind>&a_ayn;&a_z;&a_A;&a_d;&a_A;&a_r;&f_y;</ind></trd>
    </trdgrp>
    <trd lng="hu">gy&aacute;szruha</trd>    
    <trd lng="nl">rouwkledij</trd>
    <trdgrp lng="pt">
      <trd>luto</trd>,
      <trd>nojo</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_l;&c_o;&c_b;&c_a;</trd>
  <trd lng="bo">&#xF66;&#xF90;&#xF74;&#xF0B;&#xF55;&#xF74;&#xF44;&#xF0B;&#xF42;&#xF51;&#xF53;&#xF0B;&#xF5E;&#xF74;&#xF0B;</trd>
  <trd lng="cs">smutek</trd>
  <trd lng="fr">deuil</trd>
  <trdgrp lng="ja">
    <trd>&#x6B7B;&#x5225;&#x306E;&#x60B2;&#x3057;&#x307F;  [<ind>&#x3057;&#x3079;&#x3064;&#x306E;&#x304B;&#x306A;&#x3057;&#x307F;</ind>]</trd>,
    <trd>&#x54C0;&#x60BC;  [<ind>&#x3042;&#x3044;&#x3068;&#x3046;</ind>]</trd>,
    <trd>&#x55AA;  [<ind>&#x3082;</ind>]</trd>,
    <trd>&#x55AA;&#x670D;  [<ind>&#x3082;&#x3075;&#x304F;</ind>]</trd>,
    <trd>&#x55AA;&#x7AE0;  [<ind>&#x3082;&#x3057;&#x3087;&#x3046;</ind>]</trd>,
    <trd>&#x55AA;&#x4E2D;  [<ind>&#x3082;&#x3061;&#x3085;&#x3046;</ind>]</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_a;&c_u;&c_r;</trd>
  <trd lng="sk">sm&uacute;tok</trd>
  <trdgrp lng="uk">
    <trd>&#x436;&#x430;&#x43B;&#x43E;&#x431;&#x430;</trd>,
    <trd>&#x441;&#x43A;&#x43E;&#x440;&#x431;&#x43E;&#x442;&#x430;</trd>,
    <trd>&#x442;&#x440;&#x430;&#x443;&#x440;</trd>
  </trdgrp>
</drv>

<drv mrk="funebr.0a">
  <kap><ofc>*</ofc><tld/>a <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="&AdE;/fundamento/universala_vortaro.html#av-5033">funebr'a</url></lok></fnt></kap>
  <snc mrk="funebr.0a.sente">
    <dif>
      Mal&gcirc;oja pro morto, malfeli&ccirc;ego:
      <ekz>
        <tld/>a sento, mieno, rakonto.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>trauervoll</trd>,
      <trd>d&uuml;ster</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>funereal</trd>,
      <trd>dismal</trd>,
      <trd>gloomy</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_s;&a_w;&f_g;&a_w;&a_A;&a_r;</trd>,
      <trd>&a_ayn;&a_z;&a_A;&a_d;&a_A;&a_r;</trd>,
      <trd>&a_m;&a_A;&a_t;&a_m;&zwnj;&a_d;&f_y;&a_d;&a_h;</trd>
    </trdgrp>
    <trd lng="nl">rouwend</trd>
  </snc>
  <snc mrk="funebr.0a.more">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a vesto<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a procesio<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a ceremonio;
      </ekz>
      <ekz>
        oni <klr>...</klr> pendigu <tld/>ajn kurtenojn
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 63a, p. 378a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Trauer-</trd>,
      <trd>Begr&auml;bnis-</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>funereal</trd>,
      <trd>funeral</trd>,
    </trdgrp>
    <trd lng="hu">gy&aacute;sz-</trd>    
    <trdgrp lng="nl">
      <trd>rouw-</trd>,
      <trd>begrafenis-</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_l;&c_o;&c_b;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>funebr&aacute;ln&iacute;</trd>,
    <trd>poh&rcaron;ebn&iacute;</trd>,
    <trd>smute&ccaron;n&iacute;</trd>,
    <trd>truchliv&yacute;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>de <ind>deuil</ind></trd>,
    <trd>endeuill&eacute;</trd>,
    <trd>fun&egrave;bre</trd>
    <trd>fun&egrave;r&egrave;re</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x54C0;&#x60BC;&#x306E;  [<ind>&#x3042;&#x3044;&#x3068;&#x3046;&#x306E;</ind>]</trd>,
    <trd>&#x60B2;&#x5606;&#x306B;&#x304F;&#x308C;&#x305F;</trd>,
    <trd>&#x55AA;&#x306E;  [<ind>&#x3082;&#x306E;</ind>]</trd>,
    <trd>&#x846C;&#x5F0F;&#x306E;  [<ind>&#x305D;&#x3046;&#x3057;&#x304D;&#x306E;</ind>]</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_a;&c_u;&c_r;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">pohrebn&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x436;&#x430;&#x43B;&#x43E;&#x431;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x441;&#x43A;&#x43E;&#x440;&#x431;&#x43E;&#x442;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x442;&#x440;&#x430;&#x443;&#x440;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x43F;&#x43E;&#x445;&#x43E;&#x440;&#x43E;&#x43D;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x43D;&#x430;&#x434;&#x433;&#x440;&#x43E;&#x431;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="funebr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="funebr.0i.sente">
    <dif>
      Interne senti <tld/>on:
      <ekz>
        <tld/>i la morton de iu<fnt>B</fnt>;
      </ekz>
      <ekz>
        kiel homo, kiu <tld/>as pri sia patrino, mi profunde mal&gcirc;ojis
        <fnt><bib>MT</bib>, <lok>&Psa; 35:14</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i pri, pro la morto de iu;
      </ekz>
      <ekz>
        <tld/>i la perditan feli&ccirc;on, pro la perdita feli&ccirc;o.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_p;&c_l;&c_a;&c_k;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_b;&c_y;&c_c;&c_mol; &c_u; <ind>&c_zh;&c_a;&c_l;&c_o;&c_b;&c_je;</ind></trd>
    </trdgrp>
    <trd lng="en">grieve</trd>
  </snc>
  <snc mrk="funebr.0i.more">
    <dif>
      Ekstere montri <tld/>on:
      <ekz>
        <tld/>i iun, pro iu<fnt>Z</fnt>, pri iu;
      </ekz>
      <ekz>
        <tld/>i per longa ceremonio la filon, la morton de la filo.
      </ekz>
    </dif>
    <trd lng="be">&c_n;&c_a;&c_s;&c_ib;&c_c;&c_mol; <ind>&c_zh;&c_a;&c_l;&c_o;&c_b;&c_u;</ind></trd>
    <trdgrp lng="en">
      <trd>weep</trd>,
      <trd>bewail</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>dr&zcaron;et smutek</trd>,
    <trd>truchlit</trd>
  </trdgrp>
  <trd lng="de">trauern</trd>
  <trd lng="en">mourn</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_w;&f_g;&a_w;&a_A;&a_r;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_ayn;&a_z;&a_A;&a_d;&a_A;&a_r;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&ecirc;tre en <ind>deuil</ind> (de)</trd>,
    <trd>porter le <ind>deuil</ind> (de)</trd>,
    <trd>pleurer<klr tip="ind"> (qn, la mort de qn)</klr></trd>
  </trdgrp>
  <trd lng="hu">(meg)<ind>gy&aacute;szol</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x6DF1;&#x304F;&#x60B2;&#x3057;&#x3080;  [<ind>&#x3075;&#x304B;&#x304F;&#x304B;&#x306A;&#x3057;&#x3080;</ind>]</trd>,
    <trd>&#x6B7B;&#x3092;&#x60BC;&#x3080;  [<ind>&#x3057;&#x3092;&#x3044;&#x305F;&#x3080;</ind>]</trd>,
    <trd>&#x5F14;&#x3046;  [<ind>&#x3068;&#x3080;&#x3089;&#x3046;</ind>]</trd>,
    <trd>&#x55AA;&#x306B;&#x670D;&#x3059;&#x308B;  [<ind>&#x3082;&#x306B;&#x3075;&#x304F;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">rouwen</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_p;&c_l;&c_a;&c_k;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_o;&c_s;&c_i;&c_t;&c_mol; &c_t;&c_r;&c_a;&c_u;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>sm&uacute;ti&tcaron;</trd>,
    <trd>tr&uacute;chli&tcaron;</trd>,
    <trd>&zcaron;iali&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x441;&#x443;&#x43C;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43E;&#x43F;&#x43B;&#x430;&#x43A;&#x443;&#x432;&#x430;&#x442;&#x438; (&#x43D;&#x435;&#x431;&#x456;&#x436;&#x447;&#x438;&#x43A;&#x430;)</trd>,
    <trd>&#x43D;&#x43E;&#x441;&#x438;&#x442;&#x438; &#x436;&#x430;&#x43B;&#x43E;&#x431;&#x443;</trd>
  </trdgrp>
</drv>
