<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>funebr</rad>/o
</kap>
<drv mrk="funebr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="funebr.0o.sento">
    <dif>
      Mal&gcirc;oja sento pro ies morto, pro malfeli&ccirc;ego:
      <ekz>
        estas granda <tld/>o en la domo, en la tuta lando.
      </ekz>
    </dif>
    <trd lng="de">Trauer</trd>
    <trd lng="hu">gy&aacute;sz</trd>    
    <trd lng="nl">rouw</trd>
  <trdgrp lng="pt">
    <trd>luto</trd>,
    <trd>nojo</trd>
  </trdgrp>
  </snc>
  <snc mrk="funebr.0o.moro">
    <dif>
      Eksteraj montroj de tia mal&gcirc;ojo; <tld/>a ceremonio:
      <ekz>
        piuloj enterigis Stefanon, kaj faris pri li grandan <tld/>on
        <fnt><bib>NT</bib>, <lok>&Ago; 8:2</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>o da&ubreve;ris unu jaron<fnt>B</fnt>;
      </ekz>
      <ekz>
        multaj personoj &ccirc;eestis la <tld/>on.
      </ekz>
    </dif>
    <trd lng="de">Trauerfeier</trd>
    <trdgrp lng="fr">
      <trd>fun&eacute;railles</trd>,
      <trd>obs&egrave;ques</trd>,
    </trdgrp>
    <trd lng="hu">gy&aacute;szol&aacute;s</trd>    
    <trd lng="nl">rouw</trd>
  <trdgrp lng="pt">
    <trd>luto</trd>,
    <trd>nojo</trd>
  </trdgrp>
  </snc>
  <snc mrk="funebr.0o.vesto">
    <dif>
      Vestoj de <tld/>o:
      <ekz>
        survesti, porti <tld/>on pri iu<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">Trauerkleidung</trd>
    <trd lng="hu">gy&aacute;szruha</trd>    
    <trd lng="nl">rouwkledij</trd>
  <trdgrp lng="pt">
    <trd>luto</trd>,
    <trd>nojo</trd>
  </trdgrp>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_l;&c_o;&c_b;&c_a;</trd>
  <trd lng="fr">deuil</trd>
  <trd lng="ru">&c_t;&c_r;&c_a;&c_u;&c_r;</trd>
</drv>

<drv mrk="funebr.0a">
  <kap><tld/>a</kap>
  <snc mrk="funebr.0a.sente">
    <dif>
      Mal&gcirc;oja pro morto, malfeli&ccirc;ego:
      <ekz>
        <tld/>a sento, mieno, rakonto.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>trauervoll</trd>,
      <trd>d&uuml;ster</trd>
    </trdgrp>
    <trd lng="nl">rouwend</trd>
  </snc>
  <snc mrk="funebr.0a.more">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a vesto<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a procesio<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a ceremonio;
      </ekz>
      <ekz>
        oni <klr>...</klr> pendigu <tld/>ajn kurtenojn
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 63a, p. 378a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Trauer-</trd>,
      <trd>Begr&auml;bnis-</trd>
    </trdgrp>
    <trd lng="hu">gy&aacute;sz-</trd>    
    <trdgrp lng="nl">
      <trd>rouw-</trd>,
      <trd>begrafenis-</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_l;&c_o;&c_b;&c_n;&c_y;</trd>
  <trdgrp lng="fr">
    <trd>de <ind>deuil</ind></trd>,
    <trd>endeuill&eacute;</trd>,
    <trd>fun&egrave;bre</trd>
    <trd>fun&egrave;r&egrave;re</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_a;&c_u;&c_r;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="funebr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="funebr.0i.sente">
    <dif>
      Interne senti <tld/>on:
      <ekz>
        <tld/>i la morton de iu<fnt>B</fnt>;
      </ekz>
      <ekz>
        kiel homo, kiu <tld/>as pri sia patrino, mi profunde mal&gcirc;ojis
        <fnt><bib>MT</bib>, <lok>&Psa; 35:14</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i pri, pro la morto de iu;
      </ekz>
      <ekz>
        <tld/>i la perditan feli&ccirc;on, pro la perdita feli&ccirc;o.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_p;&c_l;&c_a;&c_k;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_b;&c_y;&c_c;&c_mol; &c_u; <ind>&c_zh;&c_a;&c_l;&c_o;&c_b;&c_je;</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="funebr.0i.more">
    <dif>
      Ekstere montri <tld/>on:
      <ekz>
        <tld/>i iun, pro iu<fnt>Z</fnt>, pri iu;
      </ekz>
      <ekz>
        <tld/>i per longa ceremonio la filon, la morton de la filo.
      </ekz>
    </dif>
    <trd lng="be">&c_n;&c_a;&c_s;&c_ib;&c_c;&c_mol; <ind>&c_zh;&c_a;&c_l;&c_o;&c_b;&c_u;</ind></trd>
  </snc>
  <trd lng="de">trauern</trd>
  <trdgrp lng="fr">
    <trd>&ecirc;tre en <ind>deuil</ind> (de)</trd>,
    <trd>porter le <ind>deuil</ind> (de)</trd>,
    <trd>pleurer<klr tip="ind"> (qn, la mort de qn)</klr></trd>
  </trdgrp>
  <trd lng="hu">(meg)<ind>gy&aacute;szol</ind></trd>  
  <trd lng="nl">rouwen</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_p;&c_l;&c_a;&c_k;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_o;&c_s;&c_i;&c_t;&c_mol; &c_t;&c_r;&c_a;&c_u;&c_r;</trd>
  </trdgrp>
</drv>
</art>
