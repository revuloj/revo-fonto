<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: murmur.xml,v 1.33 2025/12/26 04:47:26 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>murmur</rad>/i <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="murmur.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc mrk="murmur.0i.brui">
    <uzo tip="klr">(abs.)</uzo>
    <dif>
      <ref tip="super" cel="bru.0i.soni">Brui</ref> malforte,
      malklare, obtuze, kiel fluanta akvo sur &scirc;tonoj, vento tra foliaro,
      amaso da homoj samtempe parolantaj:
      <ekz>
        la rivereto, la vento <tld/>as;
      </ekz><ekz>
        la birdoj gaje <tld/>as sur arboj;
      </ekz>
      <ekz>
        el tero senfrukta &scirc;prucanta <tld/>is fonteto
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz><ekz>
        akvo <tld/>etas en la bolkru&ccirc;o;
      </ekz>
      <ekz>
        akvo bolas, <tld/>as, sed fine &gcirc;i kuras
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        en iliaj oreloj kredeble <tld/>as ankora&ubreve; la funebra kanto
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>bruire</trd>,
      <trd>bruisser</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>mormol</trd>,
      <trd>z&uacute;g</trd>,
      <trd>csobog</trd>,
      <trd>cs&ouml;rgedezik</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd><baz>desir</baz>berdesir</trd>,
      <trd><baz>desir</baz>mendesir</trd>,
      <trd><baz>desis</baz>berdesis</trd>,
      <trd><baz>desis</baz>mendesis</trd>,
      <trd><baz>gemeresik</baz>bergemeresik</trd>,
      <trd><baz>gemerisik</baz>bergemerisik</trd>,
      <trd><baz>gemericik</baz>bergemericik</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>murmurar</trd>,
      <trd>farfalhar <klr tip="amb">(folhas)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_zh;&c_u;&c_r;&c_ch;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_sh;&c_u;&c_r;&c_sh;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_sh;&c_je;&c_l;&c_je;&c_s;&c_t;&c_je;&c_t;&c_mol;</trd>,
      <trd>&c_sh;&c_u;&c_m;&c_je;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="murmur.0i.paroleti">
    <gra><vspec>tr</vspec></gra>
    <dif>
      <ref tip="super" cel="parol.0i.diri">Paroli</ref> malla&ubreve;te:
      <ekz>
        <tld/>i minacojn;
      </ekz>
      <ekz>
        kiam <nom>David</nom> vidis, ke liaj servantoj <tld/>etas inter si, li komprenis, ke la
        infano mortis
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 12:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        demandu la anta&ubreve;diristojn kaj la sor&ccirc;istojn, la flustristojn kaj la
        <tld/>etistojn
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 8:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi bezonas min iom malvarmetigi, li <tld/>etis al si mem
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu en<tld/>is al mi tion &ccirc;i en la orelon?
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>;
      </ekz><ekz>
        post longa momento &scirc;i <tld/>is: &leftquot;vi jam estas
        dudek-sesjara...&rightquot;
        <fnt>
          <aut>Shi Chengtai</aut>,
          <vrk>La kompensa kiso</vrk>,
          <bib>Monato</bib>,
          <lok>1995:6, p. 27a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flustr.0i">flustri</ref>
    </refgrp>
    <rim>
      <em><tld lit="M"/>ante</em> oni malla&ubreve;tigas la vo&ccirc;sonon, dum
      <em>flustrante</em> oni ne uzas la
      vo&ccirc;on, sed nur siblas per lango, dentoj kaj lipoj.
    </rim>
    <trdgrp lng="fr">
      <trd>murmurer</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>mormol</trd>,
      <trd>suttog</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd><baz>gumam</baz>menggumam</trd>,
      <trd><baz>gumam</baz>bergumam</trd>,
      <trd><baz>bisik</baz>berbisik</trd>,
      <trd><baz>bisik</baz>berbisik-bisik</trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_o;&c_r;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="murmur.0i.protesti">
    <uzo tip="klr">(abs.)</uzo>
    <dif>
      <ref tip="dif" cel="grumbl.0i">Grumbli</ref>:
      <ekz>
        &leftquot;iru tuj!&rightquot; &dash; la filino iris, sed &ccirc;iam <tld/>ante
        <fnt>
          <bib>F</bib>
          <lok>&FE; 19</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i tra la dentoj
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>egas la urso, sed danci &gcirc;i devas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la popolo ek<tld/>is kontra&ubreve; Moseo, dirante: kiam ni trinkos
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 15:24</lok>
        </fnt>?
      </ekz>
      <ekz>
        ili anka&ubreve; ricevis po unu denaro kaj tion ricevinte, ili <tld/>is kontra&ubreve; la
        domomastro
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 20:11</lok>
        </fnt>;
      </ekz><ekz>
        <ind>sur la ventro veluro, en la ventro <tld/>o</ind>
        <klr>(ekstere &scirc;ajnas luksa sed efektive estas
        malprospera)</klr>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="de">au&szlig;en hui, innen pfui</trd>
      </ekz>
    </dif>
    <trd lng="de">grummeln</trd>
    <trdgrp lng="fr">
      <trd>bougonner</trd>,
      <trd>grommeler</trd>,
      <trd>grogner<klr tip="ind"> (r&eacute;criminer)</klr></trd>,
      <trd>r&acirc;ler<klr tip="ind"> (r&eacute;criminer)</klr></trd>,
      <trd>r&eacute;criminer</trd>,
      <trd>ronchonner</trd>,
      <trd>rousp&eacute;ter</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>d&ouml;rm&ouml;g</trd>,
      <trd>morog</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd><baz>gerutu</baz>menggerutu</trd>,
      <trd><baz>comel</baz>mencomel</trd>,
      <trd><baz>sungut</baz>bersungut</trd>,
      <trd><baz>sungut</baz>bersungut-sungut</trd>,
      <trd>rengus</trd>,
      <trd><baz>rengus</baz>merengus</trd>,
      <trd>rengut</trd>,
      <trd><baz>rengut</baz>merengut</trd>,
      <trd>rungut</trd>,
      <trd><baz>rungut</baz>merungut</trd>,
      <trd><baz>rungut</baz>berungut</trd>,
      <trd>berengut</trd>,
      <trd><baz>berengut</baz>memberengut</trd>,
      <trd>berungut</trd>,
      <trd><baz>berungut</baz>memberungut</trd>
    </trdgrp>
    <trd lng="pt">resmungar</trd>
    <trd lng="ru">&c_r;&c_o;&c_p;&c_t;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_u;&c_r;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_c;&c_u;&c_r;&c_ch;&c_e;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_k;&c_a;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_m;&c_y;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_a;&c_m;&c_a;&c_c;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_u;&c_m;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_o;&c_r;&c_g;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bru&ccaron;et</trd>,
    <trd>bublat</trd>,
    <trd>hu&ccaron;et</trd>,
    <trd>mru&ccaron;et</trd>,
    <trd>skuhrat</trd>,
    <trd>&scaron;um&ecaron;t</trd>
  </trdgrp>
  <trd lng="de">murmeln</trd>
  <trdgrp lng="en">
    <trd>murmur</trd>,
    <trd>mutter</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12388;&#12406;&#12420;&#x304F;</trd>,
    <trd>&#12374;&#x308F;&#12417;&#x304F;</trd>,
    <trd>&#12406;&#12388;&#x304F;&#12373;&#x8A00;&#12358;  <pr>&#12406;&#12388;&#x304F;&#12373;&#12356;&#12358;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>murmelen</trd>,
    <trd>brommen</trd>,
    <trd>morren</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>szepta&cacute;</trd>,
    <trd>mrucze&cacute;</trd>,
    <trd>szemra&cacute;</trd>,
    <trd>brz&eogonek;cze&cacute;</trd>,
    <trd>warcze&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>&#537;opti</trd>,
    <trd>toarce</trd>,
    <trd>susura</trd>,
    <trd>b&acirc;z&acirc;it</trd>,
    <trd>m&acirc;r&abreve;i</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hundra&tcaron;; &zcaron;blnkota&tcaron;; &scaron;tebota&tcaron;</trd>,
    <trd>mrmla&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_z;&c_ju;&c_r;&c_ch;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_sh;&c_a;&c_r;&c_u;&c_d;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_sh;&c_je;&c_l;&c_je;&c_s;&c_t;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_sh;&c_je;&c_p;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_sh;&c_je;&c_p;&c_o;&c_t;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_m;&c_o;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_m;&c_o;&c_t;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_m;&c_u;&c_r;&c_m;&c_u;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_m;&c_u;&c_r;&c_m;&c_o;&c_t;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_m;&c_i;&c_m;&c_r;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21891;&#21891; <pr>n&aacute;nn&aacute;n</pr></trd>,
    <trd>&#x6F7A;&#x6F7A; <pr>ch&aacute;nch&aacute;n</pr></trd>,
    <trd>&#x546B; <pr>ti&egrave;</pr></trd>,
    <trd>&#x4F4E;&#x541F; <pr>d&imacron;y&iacute;n</pr></trd>,
    <trd>&#21653;&#x54DD; <pr>g&umacron;nong</pr></trd>,
    <trd>&#21653;&#22117; <pr>g&umacron;nong</pr></trd>,
    <trd>&#x56D4;&#x56D4; <pr>n&amacron;ngnang</pr></trd>,
    <trd>&#x8B9F; <pr>d&uacute;</pr></trd>,
    <trd>&#x2E64B; <pr>d&uacute;</pr></trd>,
    <trd>&#21653; <pr>g&umacron;</pr></trd>,
    <trd>&#x8B9F; <pr>d&uacute;</pr></trd>,
    <trd>&#x4F4E;&#x8A9E; <pr>d&imacron;y&#x1D4;</pr></trd>,
    <trd>&#x4F4E;&#x8BED; <pr>d&imacron;y&#x1D4;</pr></trd>,
    <trd>&#x79C1;&#x8A9E; <pr>s&imacron;y&#x1D4;</pr></trd>,
    <trd>&#x79C1;&#x8BED; <pr>s&imacron;y&#x1D4;</pr></trd>,
    <trd>&#x56D4; <pr>n&amacron;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="murmur.0o">
  <kap><tld/>o</kap>
  <snc mrk="murmur.0o.SON">
    <dif>
      Malla&ubreve;ta <ref tip="lst" cel="son.0o" lst="voko:sonoj">sono</ref>
      de io a&ubreve; iu obtuze bruanta:
      <ekz>
        <tld/>o de admiro kuris tra la teatro
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de la pa&scirc;oj de la infanterio, mar&scirc;anta takte
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        eksonis la akordoj <klr>[&#8230;]</klr> kuni&gcirc;ante kun la <tld/>o de la arboj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        lia akra a&ubreve;do perceptis venantan de sube, <klr>[&#8230;]</klr>
        obtuzan <tld/>on
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 2a, p. 28a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_u;&c_r;&c_ch;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_ch;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_m;&c_y;&c_t;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_sh;&c_a;&c_m;&c_a;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_sh;&c_u;&c_m;</trd>,
    <trd>&c_sh;&c_o;&c_l;&c_a;&c_h;</trd>,
    <trd>&c_sh;&c_o;&c_r;&c_g;&c_a;&c_t;</trd>,
    <trd>&c_g;&c_u;&c_l;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bubl&aacute;n&iacute;</trd>,
    <trd>hu&ccaron;en&iacute;</trd>,
    <trd>mru&ccaron;en&iacute;</trd>,
    <trd>obt&ecaron;&zcaron;uj&iacute;c&iacute; hluk</trd>,
    <trd>skuhr&aacute;n&iacute;</trd>,
    <trd>&scaron;um&ecaron;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Murmeln</trd>
  <trd lng="en">murmur</trd>
  <trdgrp lng="fr">
    <trd>bougonnement</trd>,
    <trd>murmure</trd>,
    <trd>ronchonnement</trd>,
    <trd>rumeur<klr tip="amb"> (bruit confus)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>mormol&aacute;s</trd>,
    <trd>csobog&aacute;s</trd>,
    <trd>z&uacute;g&aacute;s</trd>,
    <trd>cs&ouml;rgedez&eacute;s</trd>,
    <trd>morg&aacute;s</trd>,
    <trd>d&ouml;rm&ouml;g&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>desir</trd>,
    <trd>desiran</trd>,
    <trd>desis</trd>,
    <trd>desisan</trd>,
    <trd>gumam</trd>,
    <trd>gumaman</trd>,
    <trd>bisik</trd>,
    <trd>bisikan</trd>,
    <trd>gerutu</trd>,
    <trd>sungut</trd>,
    <trd>comel</trd>,
    <trd>gemeresik</trd>,
    <trd>gemerisik</trd>,
    <trd>gemericik</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12388;&#12406;&#12420;&#x304D;</trd>,
    <trd>&#12374;&#x308F;&#12417;&#x304D;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pomruk</trd>,
    <trd>szemranie</trd>,
    <trd>szmer</trd>,
    <trd>gwar</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>fream&abreve;t</trd>,
    <trd>f&icirc;&#537;&icirc;ial&abreve;</trd>,
    <trd>zgomot</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_je;&c_l;&c_je;&c_s;&c_t;</trd>,
    <trd>&c_sh;&c_o;&c_r;&c_o;&c_h;</trd>,
    <trd>&c_zh;&c_u;&c_r;&c_ch;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_r;&c_o;&c_p;&c_o;&c_t;</trd>,
    <trd>&c_b;&c_o;&c_r;&c_m;&c_o;&c_t;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_g;&c_u;&c_l; &c_g;&c_o;&c_l;&c_o;&c_s;&c_o;&c_v;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bru&ccaron;anie <klr>(nespokojn&eacute;)</klr></trd>,
    <trd>mru&ccaron;anie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_z;&c_ju;&c_r;&c_k;&c_ib;&c_t;</trd>,
    <trd>&c_sh;&c_u;&c_r;&c_h;&c_ib;&c_t;</trd>,
    <trd>&c_sh;&c_je;&c_l;&c_je;&c_s;&c_t;</trd>,
    <trd>&c_sh;&c_je;&c_p;&c_ib;&c_t;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_m;&c_o;&c_t;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_m;&c_o;&c_t;&c_ib;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_m;&c_u;&c_r;&c_m;&c_u;&c_t;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_m;&c_u;&c_r;&c_m;&c_o;&c_t;&c_ib;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x55B3;&#x55B3; <pr>ch&amacron;cha</pr></trd>,
    <trd>&#22017; <pr>q&imacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="murmur.0egi">
  <kap><tld/>egi</kap>
  <snc mrk="murmur.0egi.SON">
    <dif>
      Tre la&ubreve;te, obtuze a&ubreve; konfuze <ref tip="super" cel="bru.0i.soni">brui</ref>
      a&ubreve; <ref tip="super" cel="blek.0i.beste">bleki</ref>:
      <ekz>
        la leono ek<tld/>egis kaj foriris
        <fnt>
          <bib>FK</bib>,
          <lok>La deveno de la virino</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la tegmento sidis vivanta blanka urso kaj <tld/>egis
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">brummen</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_u;&c_r;&c_ch;&c_a;&c_c;&c_mol;</trd>,
