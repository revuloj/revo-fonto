<drv mrk="murmur.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="murmur.0eto.SON">
    <dif>
      Malla&ubreve;ta, neklara brueto:
      <ekz>
        mi a&ubreve;das <tld/>eton en la bran&ccirc;oj
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        sen <tld/>eto falis de la arboj la folioj, &ccirc;ie regis kvieto
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        silento interrompata de la <tld/>etoj de la fontano
        <fnt>
          <bib>FdO</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>eto de admiro traflugis inter la &ccirc;eestantoj
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 1a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Gemurmel</trd>,
      <trd>Rascheln</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_u;&c_r;&c_k;&c_a;&c_t;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_sh;&c_a;&c_m;&c_a;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
