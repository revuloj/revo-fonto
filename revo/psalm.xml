<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: psalm.xml,v 1.25 2024/02/17 11:11:10 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>psalm</rad>/o
</kap>

<drv mrk="psalm.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">REL</uzo>
  <snc>
    <dif>
      Hebrea religia kantiko:
      <ekz>
        kantado de Pre&gcirc;o de Abrahamo kaj de kelkaj
        <tld/>oj<fnt><aut>Zofia
        Banet-Fornalowa</aut>: <vrk>Kadi&scirc; por Jedwabne</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 1a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_s;&c_a;&c_l;&c_o;&c_m;</trd>,
    <trd>&c_p;&c_s;&c_a;&c_l;&c_mol;&c_m;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_s;&c_a;&c_l;&c_t;&c_i;&c_r;</trd>
  <trd lng="cs">&zcaron;alm</trd>
  <trd lng="de">Psalm</trd>
  <trd lng="en">psalm</trd>
  <trdgrp lng="fr">
    <trd>psaume</trd>
  </trdgrp>
  <trd lng="hu">zsolt&aacute;r</trd>
  <trdgrp lng="ja">
    <trd>&#x8056;&#x6B4C;  <pr>&#x305B;&#x3044;&#x304B;</pr></trd>,
    <trd>&#x8A69;&#x7BC7;&#x6B4C;  <pr>&#x3057;&#x3078;&#x3093;&#x3046;&#x305F;</pr></trd>
  </trdgrp>
  <trd lng="nl">psalm</trd>
  <trd lng="pl">psalm</trd>
  <trd lng="pt">salmo</trd>
  <trd lng="ro">psalm</trd>
  <trd lng="ru">&c_p;&c_s;&c_a;&c_l;&c_o;&c_m;</trd>
  <trd lng="sk">&zcaron;alm</trd>
  <trd lng="sv">psalm</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_s;&c_a;&#x43B;&#x43E;&#x43C;</trd>,
    <trd>&#x43F;&c_s;&c_a;&#x43B;&#x44C;&#x43C;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5723;&#x6B4C; <pr>sh&#xE8;ngg&#x113;</pr></trd>,
    <trd>&#x8056;&#x6B4C; <pr>sh&#xE8;ngg&#x113;</pr></trd>,
    <trd>&#x8B9A;&#x7F8E;&#x8A69; <pr>z&#xE0;nm&#x11B;ish&#x12B;</pr></trd>,
    <trd>&#x8D5E;&#x7F8E;&#x8BD7; <pr>z&#xE0;nm&#x11B;ish&#x12B;</pr></trd>,
    <trd>&#x5723;&#x8BD7; <pr>sh&#xE8;ngsh&#x12B;</pr></trd>,
    <trd>&#x8056;&#x8A69; <pr>sh&#xE8;ngsh&#x12B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="psalm.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kanti <tld/>on.
    </dif>
      <trd lng="be">&c_s;&c_mol;&c_p;&c_ja;&c_v;&c_a;&c_c;&c_mol; <ind>&c_p;&c_s;&c_a;&c_l;&c_mol;&c_m;&c_y;</ind></trd>
    <trd lng="hu">zsolt&aacute;rt &eacute;nekel</trd>
    <trd lng="ru">&c_p;&c_je;&c_t;&c_mol; <ind>&c_p;&c_s;&c_a;&c_l;&c_o;&c_m;</ind></trd>
  </snc>
  <snc>
    <dif>
      Unutone kanti, kvaza&ubreve; <tld/>ante.
    </dif>
    <trd lng="be">&c_s;&c_mol;&c_p;&c_ja;&c_v;&c_a;&c_c;&c_mol; &c_m;&c_a;&c_n;&c_a;&c_t;&c_o;&c_n;&c_n;&c_a; <klr>(&c_n;&c_ib;&c_b;&c_y; &c_p;&c_s;&c_a;&c_l;&c_mol;&c_m;&c_y;)</klr></trd>
    <trdgrp lng="en">
      <trd>to chant</trd>,
      <trd>to sing a canticle</trd>
    </trdgrp>
    <trd lng="hu">k&aacute;nt&aacute;l</trd>
    <trd lng="ru">&c_p;&c_je;&c_t;&c_mol; &c_m;&c_o;&c_n;&c_o;&c_t;&c_o;&c_n;&c_n;&c_o;<klr> (&c_k;&c_a;&c_k;
    &c_p;&c_s;&c_a;&c_l;&c_m;&c_y;)</klr></trd>
  </snc>
  <trd lng="cs">zp&iacute;vat &zcaron;almy</trd>
  <trd lng="de">psalmieren</trd>
  <trdgrp lng="fr">
    <trd>psalmodier</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8A60;&#x5531;&#x3059;&#x308B; <pr>&#x3048;&#x3044;&#x3057;&#x3087;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5358;&#x8ABF;&#x306B;&#x6B4C;&#x3046;  <pr>&#x305F;&#x3093;&#x3061;&#x3087;&#x3046;&#x306B;&#x3046;&#x305F;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">psalmzingen</trd>
  <trd lng="pl">&sacute;piewa&cacute;  <ind>psalm</ind></trd>
  <trd lng="ro">c&acirc;nta un  <ind>psalm</ind></trd>
  <trd lng="sk">spieva&tcaron; &zcaron;almy</trd>
  <trd lng="uk">&c_s;&#x43F;&c_ib;&c_v;&c_a;&c_t;&c_i; &#x43F;&c_s;&c_a;&#x43B;&#x43C;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x541F;&#x5531; <pr>y&#xED;nch&#xE0;ng</pr></trd>,
    <trd>&#x541F;&#x8AA6; <pr>y&#xED;ns&#xF2;ng</pr></trd>,
    <trd>&#x541F;&#x8BF5; <pr>y&#xED;ns&#xF2;ng</pr></trd>,
    <trd>&#x541F; <pr>y&#xED;n</pr></trd>,
    <trd>&#x552B; <pr>y&#xED;n</pr></trd>
  </trdgrp>
</drv>
</art>
