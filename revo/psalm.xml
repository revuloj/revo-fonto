<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>psalm</rad>/o
</kap>
<drv mrk="psalm.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">REL</uzo>
  <snc>
    <dif>
      Hebrea religia kantiko:
      <ekz>
        kantado de Pre&gcirc;o de Abrahamo kaj de kelkaj <tld/>oj<fnt><aut>Zofia
        Banet-Fornalowa</aut>: <vrk>Kadi&scirc; por Jedwabne</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 1a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Psalm</trd>
  <trdgrp lng="fr">
    <trd>psaume</trd>
  </trdgrp>
  <trd lng="hu">zsolt&aacute;r</trd>
  <trd lng="nl">psalm</trd>
  <trd lng="pt">salmo</trd>
  <trd lng="ru">&c_p;&c_s;&c_a;&c_l;&c_o;&c_m;</trd>
  <trd lng="sv">psalm</trd>
</drv>

<drv mrk="psalm.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kanti <tld/>on.
    </dif>
    <trd lng="hu">zsolt&aacute;rt &eacute;nekel</trd>
    <trd lng="ru">&c_p;&c_je;&c_t;&c_mol; <ind>&c_p;&c_s;&c_a;&c_l;&c_o;&c_m;</ind></trd>
  </snc>
  <snc>
    <dif>
      Unutone kanti, kvaza&ubreve; <tld/>ante.
    </dif>
    <trd lng="hu">k&aacute;nt&aacute;l</trd>
    <trd lng="ru">&c_p;&c_je;&c_t;&c_mol; &c_m;&c_o;&c_n;&c_o;&c_t;&c_o;&c_n;&c_n;&c_o;<klr> (&c_k;&c_a;&c_k; &c_p;&c_s;&c_a;&c_l;&c_m;&c_y;)</klr></trd>
  </snc>
  <trd lng="de">psalmieren</trd>
  <trdgrp lng="fr">
    <trd>psalmodier</trd>
  </trdgrp>
  <trd lng="nl">psalmzingen</trd>
</drv>
</art>
