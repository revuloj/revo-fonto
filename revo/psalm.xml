<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>psalm</rad>/o
</kap>
<drv mrk="psalm.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">REL</uzo>
  <snc>
    <dif>
      Hebrea religia kantiko.
    </dif>
  </snc>
  <trd lng="de">Psalm</trd>
  <trd lng="nl">psalm</trd>
  <trd lng="ru">&c_p;&c_s;&c_a;&c_l;&c_o;&c_m;</trd>
  <trd lng="sv">psalm</trd>
</drv>

<drv mrk="psalm.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kanti <tld/>on.
    </dif>
  </snc>
  <snc>
    <dif>
      Unutone kanti, kvaza&ubreve; <tld/>ante.
    </dif>
  </snc>
  <trd lng="de">psalmieren</trd>
  <trd lng="nl">psalmzingen</trd>
</drv>
</art>
