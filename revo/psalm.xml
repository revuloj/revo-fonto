<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>psalm</rad>/o
</kap>
<drv mrk="psalm.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">REL</uzo>
  <snc>
    <dif>
      Hebrea religia kantiko:
      <ekz>
        kantado de Pre&gcirc;o de Abrahamo kaj de kelkaj <tld/>oj<fnt><aut>Zofia
        Banet-Fornalowa</aut>: <vrk>Kadi&scirc; por Jedwabne</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 1a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Psalm</trd>
  <trdgrp lng="fr">
    <trd>psaume</trd>
  </trdgrp>
  <trd lng="hu">zsolt&aacute;r</trd>
  <trd lng="nl">psalm</trd>
  <trd lng="ru">&c_p;&c_s;&c_a;&c_l;&c_o;&c_m;</trd>
  <trd lng="sv">psalm</trd>
</drv>

<drv mrk="psalm.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kanti <tld/>on.
    </dif>
    <trdgrp lng="hu">
      <trd>zsolt&aacute;rt &eacute;nekel</trd>,
      <trd>zsolozsm&aacute;zik</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Unutone kanti, kvaza&ubreve; <tld/>ante.
    </dif>
    <trd lng="hu">k&aacute;nt&aacute;l</trd>
  </snc>
  <trd lng="de">psalmieren</trd>
  <trdgrp lng="fr">
    <trd>psalmodier</trd>
  </trdgrp>
  <trd lng="nl">psalmzingen</trd>
</drv>
</art>
