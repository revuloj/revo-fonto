<art mrk="$Id: antolo.xml,v 1.32 2025/03/26 20:16:16 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>antologi</rad>/o
<fnt><bib>JED</bib></fnt></kap>
<drv mrk="antolo.0o">
  <kap><tld/>o</kap>
  <snc mrk="antolo.0o.BELE">
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref tip="lst" lst="voko:komunikoj" cel="tekst.0aro">Tekstaro</ref>,
      libro en kiu estas kolektitaj arte valoraj prozaj a&ubreve; poeziaj verkoj
      a&ubreve; fragmentoj el beletraj verkoj de diversaj
      a&ubreve;toroj:
      <ekz>
        <tld/>o el sepdek tri poemoj
        <fnt>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2003/008065.html">Finaj agordoj</url></vrk>
          <bib>Monato</bib>
          <lok>2003/10, p. 22</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de elektitaj gaelaj poemoj verkitaj en la tuta 20a jarcento
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2003/007935.html">Ne plu krios la koko</url></vrk>
          <bib>Monato</bib>
          <lok>2003/10, p. 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ctl>Esperanta <tld/>o: poemoj 1887-1981</ctl>
        <fnt><vrk>Esperanta antologio: poemoj 1887-1981. &dash; Roterdamo:
        Universala Esperanto-Asocio, 1984</vrk></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="digxest.0o">di&gcirc;esto</ref>,
       <ref cel="epitom.0o">epitomo</ref>,
       <ref cel="kresto.0o">krestomatio</ref>
    </refgrp>
  </snc>
  <snc mrk="antolo.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Simila kolekto de verkoj muzikaj, belartaj, kinaj ktp.
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Anka&#365; eblas diri (elektita) <ke>verkaro</ke>.
  </rim>

  <trd lng="be">&c_a;&c_n;&c_t;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_a;&c_n;&c_t;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="ca">antologia</trd>
  <trdgrp lng="cs">
    <trd>antologie</trd>,
    <trd>v&yacute;bor z liter.</trd>
  </trdgrp>
  <trd lng="de">Anthologie</trd>
  <trd lng="en">anthology</trd>
  <trd lng="es">antolog&iacute;a</trd>
  <trdgrp lng="fa">
    <trd>&f_g;&a_z;&f_y;&a_d;&a_h;</trd>,
    <trd>&f_g;&a_l;&f_ch;&f_y;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">anthologie</trd>
  <trdgrp lng="he">
    <trd>&#1488;&#1463;&#1504;&#1514;&#1493;&#1465;&#1500;&#1493;&#1465;&#1490;&#1460;&#1497;&#1464;&#1492;</trd>,
    <trd>&#1502;&#1489;&#1495;&#1512; &#1497;&#1510;&#1497;&#1512;&#1493;&#1514;</trd>
  </trdgrp>
  <trd lng="hu">antol&oacute;gia</trd>
  <trd lng="it">antologia</trd>
  <trd lng="ja">&#25991;&#23398;&#36984;&#38598;  <pr>&#12406;&#12435;&#12364;&#12367;&#12379;&#12435;&#12375;&#12421;&#12358;</pr></trd>
  <trdgrp lng="nl">
    <trd>bloemlezing</trd>,
    <trd>antologie</trd>
  </trdgrp>
  <trd lng="pl">antologia</trd>
  <trd lng="ro">antologie</trd>
  <trd lng="ru">&c_a;&c_n;&c_t;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>antol&oacute;gia</trd>,
    <trd>antol&oacute;gia</trd>,
    <trd>liter&aacute;rny v&yacute;ber</trd>,
    <trd>zborn&iacute;k</trd>
  </trdgrp>
  <trd lng="uk">&c_a;&#1085;&c_t;&#1086;&#1083;&#1086;&c_g;&c_ib;&#1103;</trd>
</drv>
