<art mrk="$Id: celeri.xml,v 1.29 2022/04/26 16:27:51 revo Exp $">
<kap>
<ofc>2</ofc>
<rad>celeri</rad>/o
</kap>
<drv mrk="celeri.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
       Specio de <ref tip="super" cel="api.0o">apio</ref>
       (<trd lng="la">Apium graveolens</trd>), kulturita kiel
       legomo, unu varia&jcirc;o havas dikan tuberon, kiun oni man&gcirc;as kutime
       kuirita:
      <ekz>
        en la kuirarto uzatas la (muelitaj) semoj, folioj kaj la bulbo
        de
        la planto, <tld/>o fajnigas &ccirc;efe supojn kaj salatojn, &ccirc;ar la
        kruda bulbo estas malbone digestebla, oni man&gcirc;as &gcirc;in &ccirc;efe
        kuirita
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=751">Vera
          celerio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        en la retejo estas 8 receptoj kun <tld/>o
        <fnt>
          <vrk><url ref="https://apetito.ikso.net/ingredienco/celerio">Celerio |
          Bonan Apetiton</url></vrk>
        </fnt>.
      </ekz>  
    </dif>
    <bld lok="&WCU;/thumb/3/38/Knolselderij_knol_%28Apium_graveolens_var._rapaceum%29_%27Dolvi%27.jpg/377px-Knolselderij_knol_%28Apium_graveolens_var._rapaceum%29_%27Dolvi%27.jpg" prm="CC-BY-SA-3.0">tubero de ~o (Apium graveolens var. rapaceum)
    
        <fnt>
          <aut>Rasbak, propra verko</aut>
          <lok><url ref="&WCW;/File:Knolselderij_knol_(Apium_graveolens_var._rapaceum)_%27Dolvi%27.jpg">&WCW;/File:Knolselderij_knol_(Apium_graveolens_var._rapaceum)_%27Dolvi%27.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_s;&c_ja;&c_l;&c_je;&c_r;&c_a;</trd>
  <trd lng="br">ach(enn)</trd>
  <trd lng="ca">api</trd>
  <trd lng="cs">celer</trd>
  <trd lng="de">Sellerie</trd>
  <trd lng="el">&sigma;&epsilon_ton;&lambda;&jota;&ny;&omikron;</trd>
  <trd lng="en">celery</trd>
  <trd lng="es">apio</trd>
  <trd lng="fa">&f_k;&a_r;&a_f;&a_s;</trd>
  <trd lng="fr">c&eacute;leri</trd>
  <trd lng="he">&#x5E1;&#x5DC;&#x5E8;&#x5D9;</trd>
  <trd lng="hu">zeller</trd>
  <trd lng="id">seledri</trd>
  <trd lng="it">sedano</trd>
  <trd lng="nl">selderij</trd>
  <trd lng="pl">seler <klr>zwyczajny</klr></trd>
  <trd lng="pt">aipo</trd>
  <trd lng="ru">&c_s;&c_je;&c_l;&c_mol;&c_d;&c_je;&c_r;&c_je;&c_j;</trd>
  <trd lng="sk">zeler</trd>
  <trd lng="sv">selleri</trd>
  <trd lng="tr">kereviz</trd>
  <trd lng="uk">&#x441;&#x435;&#x43B;&#x435;&#x440;&#x430;</trd>
</drv>
