<drv mrk="seri.0o_de_funkcioj">
  <kap><tld/>o de funkcioj<fnt><bib>MatStokTerm</bib><lok>p.
  28</lok></fnt>, <var><kap>funkci<tld/>o</kap></var></kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="seri.0o.MAT"><tld lit="S"/>o</ref>, kies
      termoj estas reelaj a&ubreve; kompleksaj
      <ref cel="funkci.0o.MAT">funkcioj</ref>.
      <ekz>
        M-provo de <nom>Weierstrass</nom> estas analogo de la kompara
        provo por malfinia <tld/>o de funkcioj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=261744">Konver&gcirc;a
          serio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la malkompono de periodaj funkcioj en konver&gcirc;a serio de
        sinusoidaj funkcioj, nomata serio de <nom>Fourier</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=132195">Jean
          Baptiste Joseph Fourier</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_e;&c_r;&c_a;&c_g; &c_f;&c_u;&c_n;&c_k;&c_c;&c_y;&c_j;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>set v tenise</trd>,
    <trd>s&eacute;rie</trd>,
    <trd>&rcaron;ada</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Funktionsreihe</trd>,
    <trd>Funktionenreihe</trd>
  </trdgrp>
  <trd lng="en">function <ind>series</ind></trd>
  <trd lng="fr">s&eacute;rie de fonctions</trd>
  <trd lng="hu">f&uuml;ggv&eacute;nysor</trd>
  <trd lng="pl">szereg funkcyjny</trd>
  <trd lng="ru">&c_f;&c_u;&c_n;&c_k;&c_c;&c_i;&c_o;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; <ind>&c_r;&c_ja;&c_d;</ind></trd>
  <trd lng="sk">filmov&yacute; seri&aacute;l</trd>
</drv>

