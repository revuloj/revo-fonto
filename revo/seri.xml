<drv mrk="seri.televid0o">
  <kap>televid<tld/>o</kap>
  <snc mrk="seri.televid0o.KIN">
    <uzo tip="fak">KIN</uzo>
    <dif>
      Televidebla <ref tip="super" cel="seri.film0o.KIN">film<tld/>o</ref>:
      <ekz>
        <ctl>La vero estas tie ekstere</ctl> eble vi spektis
        iufoje tiun famkonatan televid<tld/>on el Usono
        <fnt>
          <aut>F&aacute;tima</aut>,
          <vrk><url ref="http://sekretagxardeno.blogspot.com/2005/11/la-vero-estas-tie-ekstere.html">
          La sekreta &gcirc;ardeno</url></vrk>,
          <lok>2005-11-04</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam finfine eblis spekti en mia lando lian televid<tld/>on
        Kosmo,
        mia tuta familio kvaza&ubreve;religie sidis anta&ubreve; la malnova
        televidilo, tagon post tago
        <fnt>
          <bib>Monato</bib>
          <aut>Alberto Garc&iacute;a Fumero</aut>
          <vrk><url ref="&Monato;/2014/011252.php">Scienco
          kaj religio kunfluu, ne kverelu</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Fernsehserie</trd>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_l;&c_je;&c_s;&c_e;&c_r;&c_y;&c_ja;&c_l;</trd>
  <trd lng="cs">televizn&iacute; seri&aacute;l</trd>
  <trdgrp lng="fr">
    <trd>feuilleton t&eacute;l&eacute;vis&eacute;</trd>,
    <trd>s&eacute;rie t&eacute;l&eacute;vis&eacute;e</trd>
  </trdgrp>
  <trd lng="pl">serial telewizyjny</trd>
  <trd lng="sk">telev&iacute;zny seri&aacute;l</trd>
  <trdgrp lng="zh">
    <trd>&#x8FDE;&#x7EED;&#x5267; <pr>li&#xE1;nx&#xF9;j&#xF9;</pr></trd>,
    <trd>&#x9023;&#x7E8C;&#x5287; <pr>li&#xE1;nx&#xF9;j&#xF9;</pr></trd>,
    <trd>&#x7535;&#x89C6;&#x5267; <pr>di&#xE0;nsh&#xEC;j&#xF9;</pr></trd>,
    <trd>&#x96FB;&#x8996;&#x5287; <pr>di&#xE0;nsh&#xEC;j&#xF9;</pr></trd>
  </trdgrp>
