<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&scirc;alt</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="sxalt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxalt.0i.ELE">
  <uzo tip="fak">ELE</uzo>
    <dif>
      Fermi elektran cirkviton, tiel ke la kurento povas trapasi:
      <ekz>
        <ind><tld/>i la lumon</ind>,
        <trd lng="hu">felgy&uacute;jta a villanyt</trd>
      </ekz>
      <ekz>
        la ekbrulon <klr>(de motoro)</klr>.
      </ekz>
    </dif>
    <ref tip="sin" cel="komut.0i">komuti</ref>
  </snc>
  <trd lng="be">&c_u;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_c;&c_mol; <klr>(&c_s;&c_mol;&c_v;&c_ja;&c_t;&c_l;&c_o;, &c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;)</klr></trd>
  <trd lng="de">einschalten</trd>
  <trd lng="hu">bekapcsol</trd>  
  <trd lng="ru">&c_v;&c_k;&c_l;&c_ju;&c_ch;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="nl">
    <trd>aansteken<klr tip="ind"> (licht)</klr></trd>,
    <trd>aanzetten</trd>
  </trdgrp>
  <trd lng="pt">ligar</trd>
</drv>

<drv mrk="sxalt.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="sxalt.0ilo.ELE">
  <uzo tip="fak">ELE</uzo>
    <dif>
      Aparato, provizita per prem- a&ubreve; baskulbutono, kaj servanta jen
      por <tld/>i, jen por mal<tld/>i elektran kurenton:
      <ekz>
        duvoja <tld/>ilo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_je;&c_r;&c_a;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_u;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_v;&c_y;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Schalter</trd>
  <trd lng="hu">kapcsol&oacute;</trd>  
  <trd lng="nl">schakelaar</trd>
  <trd lng="pt">interruptor</trd>
  <trd lng="ru">&c_v;&c_y;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
</drv>

<drv mrk="sxalt.mal0i">
  <kap>mal<tld/>i, el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      Malfermi elektran cirkviton, tiel ke la kurento ne povas trapasi:
      <ekz>
        mal<tld/>i la lumon,
      </ekz>
      <ekz>
        la ekbrulon de motoro,
      </ekz>
      <ekz>
        la radioaparaton;
      </ekz>
      <ekz>
        ne el<tld/>u, ni tuj dissendos la da&ubreve;rigon de la programo!
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_c;&c_mol; <klr>(&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;, &c_s;&c_mol;&c_v;&c_ja;&c_t;&c_l;&c_o;)</klr></trd>,
    <trd>&c_a;&c_d;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">ausschalten</trd>
  <trd lng="hu">kikapcsol</trd>  
  <trdgrp lng="nl">
    <trd>uitschakelen</trd>,
    <trd>afzetten</trd>,
    <trd>doven<klr tip="ind"> (licht)</klr></trd>
  </trdgrp>
  <trd lng="pt">desligar</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_k;&c_l;&c_ju;&c_ch;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_k;&c_l;&c_ju;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="sxalt.mem0ilo">
  <kap>mem<tld/>ilo</kap>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      A&ubreve;tomata <tld/>ilo, funkcianta per si mem je certaj kondi&ccirc;oj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_w;&c_t;&c_a;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y; <ind>&c_p;&c_je;&c_r;&c_a;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</ind></trd>,
    <trd>&c_a;&c_w;&c_t;&c_a;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y; <ind>&c_w;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</ind></trd>,
    <trd>&c_a;&c_w;&c_t;&c_a;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y; <ind>&c_v;&c_y;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</ind></trd>
  </trdgrp>
  <trd lng="de">automatischer <ind>Schalter</ind></trd>
  <trd lng="hu">automata <ind>kapcsol&oacute;</ind></trd>  
  <trd lng="nl">automatische <ind>schakelaar</ind></trd>
  <trd lng="ru">&c_a;&c_v;&c_t;&c_o;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; &c_v;&c_y;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
</drv>
</art>
