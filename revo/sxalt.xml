<kap>
  <ofc>9</ofc><rad>&scirc;alt</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="sxalt.0i">
  <kap><ofc>9</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxalt.0i.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Fermi elektran cirkviton, tiel ke la 
      <ref cel="kurent.0o">kurento</ref> povas trapasi:
      <ekz>
        <klr>[&scirc;i]</klr> alkurinte la pordon, <tld/>is la lumon
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is la televidon, sed je tiu horo neniu programo kaptis lian atenton
        <fnt>
          <bib>ChM</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i diskrete premis la butonon, kiu <tld/>is nevideblan magnetofonon
        <fnt>
          <bib>ChR</bib>
        </fnt>;
      </ekz>
      <ekz>
        li piedpintis al la pezaj pordoj, transe <tld/>is la komputilon la&ubreve;kutime
        <fnt>
          <bib>Monato</bib>
          <aut>Istv&aacute;n Ertl</aut>
          <vrk><url ref="https://tekstaro.com/t?nomo=artikoloj-el-monato">Jarmil&scirc;an&gcirc;o</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="komut.0i">komuti</ref>
  </snc>
  <trd lng="be">&c_u;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_c;&c_mol; 
<klr>(&c_s;&c_mol;&c_v;&c_ja;&c_t;&c_l;&c_o;,
  &c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;)
</klr></trd>
  <trd lng="de">einschalten</trd>
  <trdgrp lng="en">
    <trd>switch on</trd>,
    <trd>turn on</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>encender (un aparato el&eacute;ctrico)</trd>,
    <trd>poner en marcha (un aparato el&eacute;ctrico)</trd>,
    <trd>cerrar(un circuito el&eacute;ctrico)</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fermer<klr tip="amb"> (un circuit)</klr></trd>
    <trd>enclencher<klr tip="amb"> (un m&eacute;canisme)</klr></trd>
    <trd>allumer<klr tip="amb"> (mettre en fonctionnement)</klr></trd>
    <trd>mettre en <ind>circuit</ind></trd>
  </trdgrp>
  <trd lng="hu">bekapcsol</trd>
  <trd lng="it">accendere<klr> (dare corrente, mettere in
  funzione)</klr>
</trd>
  <trdgrp lng="pl">
    <trd>w&lstroke;&aogonek;cza&cacute;</trd>,
    <trd>zapala&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_k;&c_l;&c_ju;&c_ch;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="nl">
    <trd>aansteken<klr tip="ind"> (licht)</klr></trd>,
    <trd>aanzetten</trd>
  </trdgrp>
  <trd lng="pt">ligar</trd>
  <trdgrp lng="uk">
    <trd>&#x432;&#x43C;&#x438;&#x43A;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x43A;&#x43B;&#x44E;&#x447;&#x430;&#x442;&#x438; (&#x435;&#x43B;&#x435;&#x43A;&#x442;&#x440;&#x438;&#x43A;&#x443;</trd>,
    <trd>&#x433;&#x430;&#x437; &#x456; &#x442;.&#x43F;.)</trd>,
    <trd>&#x437;&#x430;&#x432;&#x43E;&#x434;&#x438;&#x442;&#x438; (&#x43C;&#x43E;&#x442;&#x43E;&#x440;)</trd>
  </trdgrp>
</drv>

<drv mrk="sxalt.0ilo">
  <kap><ofc>9</ofc><tld/>ilo <fnt><bib>SPV</bib></fnt></kap>
  <snc mrk="sxalt.0ilo.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Parto de aparato per kiu oni <tld/>as kaj mal<tld/>as elektran kurenton:
      <ekz>
        &scirc;i trovis la <tld/>ilon kaj tuj fermetis la okulojn pro la lumo
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[celis]</klr> paneigi la lifton de interne, diskrete tu&scirc;ante <tld/>ilon
        <fnt>
          <bib>ChR</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> palpser&ccirc;is la <tld/>ilon, post unu minuto la tuta apartamento
        estis lumplena
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        kvaza&ubreve; oni ekturnus en mia koro ian <tld/>ilon
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_je;&c_r;&c_a;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;
</trd>,
    <trd>&c_u;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_v;&c_y;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;
</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>(&c_je;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;)
    
<ind>&c_k;&c_l;&c_ju;&c_ch;</ind></trd>,
    <trd>&c_sh;&c_a;&c_l;&c_t;&c_je;&c_r;</trd>
  </trdgrp>
  <trd lng="de">Schalter</trd>
  <trd lng="en">switch</trd>
  <trdgrp lng="es">
     <trd>interruptor</trd>,
     <trd>conmutador</trd>
  </trdgrp>   
  <trd lng="fr">interrupteur</trd>
  <trd lng="hu">kapcsol&oacute;</trd>
  <trd lng="it">interruttore</trd>
  <trd lng="nl">schakelaar</trd>
  <trd lng="pl">wy&lstroke;&aogonek;cznik</trd>
  <trd lng="pt">interruptor</trd>
  <trd lng="ru">&c_v;&c_y;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_t;&c_je;&c_l;&c_mol;
</trd>
  <trd lng="uk">&#x432;&#x438;&#x43C;&#x438;&#x43A;&#x430;&#x447;</trd>
</drv>


<drv mrk="sxalt.en0i">
  <kap>
    en<tld/>i
  </kap>
  <snc>
    <dif>
      <ref tip="dif" cel="sxalt.0i">&Scirc;alti</ref>:
      <ekz>
        la filmo fini&gcirc;is, oni en<tld/>is la lumon
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiam mi en<tld/>is lampon la steletoj sur lia &ccirc;apo kvaza&ubreve; eklumis pro ia rebrilo
        <fnt>
          <aut>Spomenka &Scaron;timec</aut>
          <vrk>Ombro sur interna pejza&gcirc;o</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">einschalten</trd>
  <trd lng="en">switch on</trd>
  <trdgrp lng="uk">
    <trd>&#x437;&#x2019;&#x454;&#x434;&#x43D;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x43C;&#x438;&#x43A;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x43A;&#x43B;&#x44E;&#x447;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="sxalt.mal0i">
  <kap><ofc>9</ofc>mal<tld/>i <fnt><bib>PIV1</bib></fnt>, 
  <var><kap>el<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="fak">ELE</uzo>
    <dif>
      Malfermi elektran cirkviton, tiel ke la kurento ne povas trapasi:
      <ekz>
        &ccirc;u mi povas peti vin mal<tld/>i la aparaton
        <fnt>
          <bib>Metrop</bib>
        </fnt>?
      </ekz>
      <ekz>
        li tute forgesas mal<tld/>i la mikrofonon kaj diras plu al la dua piloto
        <klr>[&#x2026;]</klr>
        <fnt>
          <bib>KrB</bib>
        </fnt>;
      </ekz>
      <ekz>
        noktoj &scirc;ovis min sub kusenon kaj bruske el<tld/>is lumon kaj la mondon tutan
        <fnt>
          <aut>Spomenka &Scaron;timec</aut>
          <vrk> Ombro sur interna pejza&gcirc;o</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_c;&c_mol; 
<klr>(&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;,
    &c_s;&c_mol;&c_v;&c_ja;&c_t;&c_l;&c_o;)</klr></trd>,
    <trd>&c_a;&c_d;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">ausschalten</trd>
  <trdgrp lng="en">
    <trd>switch off</trd>,
    <trd>turn off</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>desconectar</trd>,
    <trd>apagar (un aparato el&eacute;ctrico)</trd>,
    <trd>parar (un aparato el&eacute;ctrico)</trd>,
    <trd>abrir (un circuito el&eacute;ctrico)</trd>
  </trdgrp>
  <trd lng="et">l&uuml;litada</trd>
  <trdgrp lng="fr">
    <trd>ouvrir<klr tip="amb"> (un circuit)</klr></trd>
    <trd>&eacute;teindre<klr tip="amb"> (un
    m&eacute;canisme)</klr></trd>
    <trd>arr&ecirc;ter<klr tip="amb"> (faire cesser le
    fonctionnement)</klr></trd>
    <trd>mettre hors <ind>circuit</ind></trd>
  </trdgrp>
  <trd lng="hu">kikapcsol</trd>
 <trd lng="it">spegnere<klr> (togliere corrente, arrestare)</klr>
</trd>
  <trdgrp lng="nl">
    <trd>uitschakelen</trd>,
    <trd>afzetten</trd>,
    <trd>doven<klr tip="ind"> (licht)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wy&lstroke;&aogonek;cza&cacute;</trd>,
    <trd>gasi&cacute;</trd>
  </trdgrp>
  <trd lng="pt">desligar</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_k;&c_l;&c_ju;&c_ch;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_k;&c_l;&c_ju;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x438;&#x43C;&#x438;&#x43A;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x438;&#x43A;&#x43B;&#x44E;&#x447;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="sxalt.for0i">
  <kap>
    for<tld/>i
  </kap>
  <snc>
    <dif>
      <tld lit="&Scirc;"/>e interrompi, ekz-e provizadon de elektro kc:
      <ekz>
        la eksterurbaj regionetoj riskos post averto esti for<tld/>ita de la elektroreto
        por protekti la stabilecon de la cetera reto
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="https://tekstaro.com/t?nomo=artikoloj-el-monato-2012-2018">Nuklea Energio &dash;
          &Ccirc;u sabotado &ccirc;e nuklea centralo?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        oni povas varme travivi la vintron kaj for<tld/>i nova&jcirc;ojn pri homoj en
        malvarmaj <klr>[domoj]</klr>
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="https://tekstaro.com/t?nomo=artikoloj-el-monato-2012-2018">Energio &dash;
          Sunelektraj kamparanoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">abschalten</trd>
  <trd lng="en">cut off</trd>
</drv>

