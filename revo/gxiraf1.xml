<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gxiraf1.xml,v 1.15 2022/03/19 19:52:45 revo Exp $">
<kap>
  <rad>&gcirc;irafed</rad>/oj
</kap>
<drv mrk="gxiraf1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="huf.par0uloj">parhufuloj</ref>
      (<trd lng="la">Giraffidae</trd>):
      <ekz>
        kune kun la okapio la &gcirc;irafo konsistigas la familion de <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=30627">&Gcirc;irafo</url></lok>
        </fnt>.
      </ekz>      
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_y;&c_r;&c_a;&c_f;&c_y; <klr tip="amb">
  (&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">gir&agrave;fids</trd>
  <trd lng="de">Giraffen</trd>
  <trd lng="en">Giraffidae</trd>
  <trd lng="fr">giraffid&eacute;s</trd>
  <trd lng="hu">zsir&aacute;ff&eacute;l&eacute;k</trd>
  <trd lng="it">giraffidi</trd>
  <trd lng="pl">&zdot;yrafowate</trd>
  <trd lng="pt">giraf&iacute;deos</trd>
  <trd lng="ru">&c_zh;&c_i;&c_r;&c_a;&c_f;&c_y; <klr tip="amb">
  (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&#x436;&#x438;&#x440;&#x430;&#x444;&#x43E;&#x432;&#x456;</trd>
</drv>
</art>
