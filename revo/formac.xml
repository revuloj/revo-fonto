<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: formac.xml,v 1.24 2023/11/10 06:07:59 revo Exp $">
<kap><rad>formaci</rad>/o
<fnt>Z</fnt></kap>
<drv mrk="formac.0o">
  <kap><tld/>o</kap>
  <snc mrk="formac.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Speciala aran&gcirc;o de la <ref tip="prt" cel="tavol.0o.KOMUNE">tavolo</ref>j
      de la tero, rilate al unu <ref cel="geolog1.0o">geologi</ref>a epoko:
      <ekz>
        <tld/>oj de marmoro kaj pluraj mineraloj, inter kiuj la rara mineralo aragonito
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2011/010212.php">La marmora groto Gadime</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        inter la aliaj fosilioj trovitaj en &ccirc;i tiu <tld/>o estas tiuj de keltoj
        <fnt>
          <vrk><url ref="https://neciklopedio.org/wiki/Pteranodonto">Pteranodonto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        nub<tld/>oj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Ar&gcirc;enta Urbo Amarganto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_f;&c_a;&c_r;&c_m;&c_a;&c_c;&c_y;&c_ja;</trd>
    <trd lng="de">Formation</trd>
    <trd lng="en"><klr tip="amb">(geological)</klr> formation</trd>
    <trdgrp lng="hu">
      <trd>r&eacute;teg</trd>,
      <trd>k&eacute;pz&odblac;dm&eacute;ny</trd>
    </trdgrp>
    <trd lng="pl">formacja <klr tip="amb">geologiczna</klr></trd>
    <trd lng="pt">forma&ccedil;&atilde;o</trd>
    <trd lng="ru">&c_f;&c_o;&c_r;&c_m;&c_a;&c_c;&c_i;&c_ja;</trd>
    <trd lng="zh">&#x8981;&#x7D20;&#x7684;&#x7D50;&#x69CB; <pr>y&#xE0;os&#xF9;deji&#xE9;g&#xF2;u</pr></trd>
</snc>
  <snc mrk="formac.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Diversa <ref tip="super" cel="arangx.0o.KOMUNE">aran&gcirc;o</ref>,
      la&ubreve; kiu ordi&gcirc;as la eroj de korpuso a&ubreve; armeo:
      <ekz>
        batala <tld/>o;
      </ekz>
      <ekz>
        la sanitaraj <tld/>oj;
      </ekz>
      <ekz>
        tamburado en <tld/>o
        <fnt>
          <vrk><url ref="&CRI;2521/2018/11/22/177s202050.htm">&leftquot;Tamburado en formacio&rightquot; enkondukita  ... </url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        pinte de tiu fenomeno staras tiu G8-&leftquot;<tld/>o&rightquot;, al kiu balda&ubreve; ali&gcirc;os anka&ubreve; aliaj
        registaroj
        <fnt>
          <bib>Monato</bib>
          <aut>Laszlo Istvan Toth</aut>
          <vrk><url ref="&Monato;/2008/009555.php">Gigantismo (3)</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="falang.0o.HIS">falango<sncref/></ref>
    <trdgrp lng="be">
      <trd>&c_z;&c_l;&c_u;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je; <klr tip="amb">(&c_v;&c_a;&c_j;&c_s;&c_k;&c_o;&c_v;&c_a;&c_je;)</klr></trd>,
      <trd>&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_mol; <klr tip="amb">(&c_v;&c_a;&c_j;&c_s;&c_k;&c_o;&c_v;&c_a;&c_ja;)</klr></trd>
    </trdgrp>
    <trd lng="de">Verband</trd>
    <trd lng="en"><klr tip="amb">(military)</klr> formation</trd>
    <trdgrp lng="hu">
      <trd>alakzat</trd>,
      <trd>fel&aacute;ll&aacute;s</trd>
    </trdgrp>
    <trd lng="pl">formacja</trd>
    <trd lng="pt">forma&ccedil;&atilde;o</trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_o;&c_j;&c_s;&c_k;&c_o;&c_v;&c_o;&c_je; &c_s;&c_o;&c_je;&c_d;&c_i;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_v;&c_o;&c_j;&c_s;&c_k;&c_o;&c_v;&c_a;&c_ja; &c_ch;&c_a;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_o;&c_d;&c_r;&c_a;&c_z;&c_d;&c_je;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>formace</trd>,
    <trd>&uacute;tvar</trd>
  </trdgrp>
  <trd lng="es">formaci&oacute;n</trd>
  <trd lng="fr">formation<klr tip="ind"> (g&eacute;ol., mil.)</klr></trd>
  <trd lng="it">formazione</trd>
  <trdgrp lng="ja">
    <trd>&#x5C64;  <pr>&#x305D;&#x3046;</pr></trd>,
    <trd>&#x968A;&#x5F62;  <pr>&#x305F;&#x3044;&#x3051;&#x3044;</pr></trd>,
    <trd>&#x9663;&#x5F62;  <pr>&#x3058;&#x3093;&#x3051;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">formatie</trd>
  <trd lng="sk">&uacute;tvar</trd>
  <trdgrp lng="uk">
    <trd>&#x444;&#x43E;&#x440;&#x43C;&#x430;&#x446;&#x456;&#x44F;</trd>,
    <trd>&#x448;&#x438;&#x43A;&#x443;&#x432;&#x430;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x441;&#x442;&#x440;&#x456;&#x439;</trd>,
    <trd>&#x432;&#x456;&#x439;&#x441;&#x44C;&#x43A;&#x43E;&#x432;&#x435; &#x437;&#x2019;&#x454;&#x434;&#x43D;&#x430;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x447;&#x430;&#x441;&#x442;&#x438;&#x43D;&#x430;</trd>
  </trdgrp>
</drv>
</art>
