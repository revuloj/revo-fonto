<kap>
  <rad>langust</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="langus.0o">
  <kap><tld/>o</kap>
  <snc mrk="langus.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
     <ref tip="dif" cel="palinu.0o">Palinuro</ref>:
      <ekz>
        vivantaj saltetantaj <tld/>oj
        <fnt>
          <aut>Hasegawa T.</aut>,
          <vrk><url ref="http://wikilivres.ca/wiki/En_%C4%88inio_Batalanta">
          En &Ccirc;inio Batalanta</url></vrk>, 
          <lok>1954</lok>
        </fnt>;
      </ekz>
      <ekz>
        rara dukolora <tld/>o kaptita en Usono
        <fnt>
          <vrk><url ref="http://esperanto.china.org.cn/node_7117779_9.htm">Naturo kaj Mediprotekto</url></vrk>
          <lok>esperanto.china.org.cn</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_n;&c_g;&c_u;&c_s;&c_t;&c_a;</trd>
  <trd lng="bg">&c_l;&c_a;&c_n;&c_g;&c_u;&c_s;&c_t;&c_a;</trd>
  <trd lng="cs">langusta</trd>
  <trd lng="de">Languste</trd>
  <trd lng="en">langouste</trd>
  <trd lng="fr">langouste</trd>
  <trd lng="hu">languszta</trd>
  <trd lng="ja">&#x30A4;&#x30BB;&#x30A8;&#x30D3;</trd>
  <trdgrp lng="nl">
    <trd>langoest</trd>,
    <trd>zeekreeft</trd>
  </trdgrp>
  <trd lng="pl">langusta</trd> 
  <trd lng="pt">lagosta</trd>
  <trd lng="ro">langust&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_n;&c_g;&c_u;&c_s;&c_t;</trd>,
    <trd>&c_l;&c_a;&c_n;&c_g;&c_u;&c_s;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">langusta</trd>
  <trd lng="uk">&#x43B;&#x430;&#x43D;&#x491;&#x443;&#x441;&#x442;</trd>
  <trdgrp lng="zh">
    <trd>&#x9F8D;&#x8766; <pr>l&#xF3;ngxi&#x101;</pr></trd>,
    <trd>&#x9F99;&#x867E; <pr>l&#xF3;ngxi&#x101;</pr></trd>
  </trdgrp>
</drv>
