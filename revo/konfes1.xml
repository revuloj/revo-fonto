<kap>
  <rad>konfesi</rad>/o
</kap>
<drv mrk="konfes1.0o">
  <kap><tld/>o</kap>
  <tezrad fak="REL"/>
  <uzo tip="fak">JUR</uzo>
  <uzo tip="fak">REL</uzo>
  <snc>
    <dif>
      Religia komunumo posedanta sian kredon, kulton kaj organizon:
      <ekz>
        katolikismo, ortodoksismo, kalvinismo estas
        kristanaj <tld/>oj;
      </ekz>
      <ekz>
        sunaismo kaj &scirc;ijaismo estas islamaj <tld/>oj;
      </ekz>
      <ekz>
        <ind>ekster<tld/>a</ind> lernejo;
        <trd lng="fr">la&iuml;c<klr tip="ind"> (adj.)</klr></trd>
      </ekz>
      <ekz>
        inter<tld/>aj rilatoj.
      </ekz>
    </dif>
    <rim>
      <ref tip="sub" cel="eklezi.0o.POL">Eklezio</ref> kutime
      implicas pli da hierarkio, <tld/>o povas tute malhavi
      disdividon en laikojn kaj pastraron.
    </rim>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_f;&c_e;&c_s;&c_ib;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>konfese</trd>,
    <trd>vyzn&#xE1;n&#xED;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bekenntnis</trd>,
    <trd>Glaubensbekenntnis</trd>,
    <trd>Konfession</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>denomination</trd>, <trd>congregation</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>confession<klr tip="ind"> (culte)</klr></trd>,
    <trd>culte</trd>,
    <trd>croyance</trd>,
    <trd>&eacute;glise<klr tip="ind"> (culte)</klr></trd>,
    <trd>foi<klr tip="ind"> (culte)</klr></trd>,
    <trd>religion</trd>
  </trdgrp>
  <trd lng="hu">felekezet</trd>
  <trdgrp lng="ja">
    <trd>&#x6559;&#x6D3E;  <pr>&#x304D;&#x3087;&#x3046;&#x306F;</pr></trd>,
    <trd>&#x5B97;&#x6D3E;  <pr>&#x3057;&#x3085;&#x3046;&#x306F;</pr></trd>
  </trdgrp>
  <trd lng="nl">belijdenis</trd>
  <trdgrp lng="pl">
    <trd>konfesja</trd>,
    <trd>wyznanie</trd>
  </trdgrp>
  <trd lng="pt">confiss&atilde;o <klr tip="amb">(rel.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_r;&c_o;&c_i;&c_s;&c_p;&c_o;&c_v;&c_je;&c_d;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_f;&c_je;&c_s;&c_s;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">vierovyznanie</trd>
  <trdgrp lng="zh">
    <trd>&#x6559;&#x4E49; <pr>ji&#xE0;oy&#xEC;</pr></trd>,
    <trd>&#x6559;&#x6D3E; <pr>ji&#xE0;op&#xE0;i</pr></trd>,
    <trd>&#x6559; <pr>ji&#xE0;o</pr></trd>,
    <trd>&#x4E3B;&#x4E49; <pr>zh&#x1D4;y&#xEC;</pr></trd>,
    <trd>&#x7DB1;&#x9818; <pr>g&#x101;ngl&#x1D0;ng</pr></trd>,
    <trd>&#x4FE1;&#x6761; <pr>x&#xEC;nti&#xE1;o</pr></trd>,
    <trd>&#x4FE1;&#x7D93; <pr>x&#xEC;nj&#x12B;ng</pr></trd>,
    <trd>&#x5766;&#x767D;&#x4FE1;&#x4EF0; <pr>t&#x1CE;nb&#xE1;ix&#xEC;ny&#x1CE;ng</pr></trd>
  </trdgrp>
</drv>
