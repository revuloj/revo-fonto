<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>konfesi</rad>/o
    </kap>
    <drv mrk="konfes1.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">JUR</uzo>
      <uzo tip="fak">REL</uzo>
      <snc>
        <dif>
          Religia asocio posedanta sian kredon, kulton kaj organizon:
          <ekz>
            katolikismo, ortodoksismo, kalvinismo estas
            kristanaj <tld/>oj;
          </ekz><ekz>
            sunaismo kaj &scirc;ijaismo estas islamaj <tld/>oj;
          </ekz><ekz>
            ekster<tld/>a lernejo;
          </ekz><ekz>
            inter<tld/>aj rilatoj.
          </ekz>
        </dif>
        <rim>
          <ref tip="sub" cel="eklezi.0o.POL">Eklezio</ref> kutime
          implicas pli da hierarkio, <tld/>o povas tute malhavi
          disdividon en laikojn kaj pastraron.
        </rim>
      </snc>
      <trdgrp lng="en">
        <trd>denomination</trd>, <trd>congregation</trd></trdgrp>
      <trdgrp lng="ru">
        <trd>&c_v;&c_je;&c_r;&c_o;&c_i;&c_s;&c_p;&c_o;&c_v;&c_je;&c_d;&c_a;&c_n;&c_i;&c_je;</trd>,
        <trd>&c_k;&c_o;&c_n;&c_f;&c_je;&c_s;&c_s;&c_i;&c_ja;</trd></trdgrp>
    </drv>
  </art>
