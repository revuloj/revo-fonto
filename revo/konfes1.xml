    <drv mrk="konfes1.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">JUR</uzo>
      <uzo tip="fak">REL</uzo>
      <snc>
        <dif>
          Religia asocio posedanta sian kredon, kulton kaj organizon:
          <ekz>
            katolikismo, ortodoksismo, kalvinismo estas
            kristanaj <tld/>oj;
          </ekz><ekz>
            sunaismo kaj &#x15D;ijaismo estas islamaj <tld/>oj;
          </ekz><ekz>
            ekster<tld/>a lernejo;
          </ekz><ekz>
            inter<tld/>aj rilatoj.
          </ekz>
        </dif>
        <ref tip="sub" cel="eklezi.0o">eklezio</ref>
      </snc>
      <trdgrp lng="en">
        <trd>denomination</trd>, <trd>congregation</trd></trdgrp>
      <trdgrp lng="ru">
        <trd>&#x432;&#x435;&#x440;&#x43E;&#x438;&#x441;&#x43F;&#x43E;&#x432;&#x435;&#x434;&#x430;&#x43D;&#x438;&#x435;</trd>,
        <trd>&#x43A;&#x43E;&#x43D;&#x444;&#x435;&#x441;&#x441;&#x438;&#x44F;</trd></trdgrp>
    </drv>
