<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mongol</rad>/a
</kap>
<drv mrk="mongol.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ano de tradicie nomada popolo de la stepoj inter Siberio kaj
      la norda &Ccirc;inujo.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_g;&c_o;&c_l;</trd>
  <trd lng="de">Mongole</trd>
  <trd lng="en">Mongolian</trd>
  <trd lng="hu">mongol</trd>
  <trd lng="nl">Mongool</trd>
  <trd lng="ru">&c_m;&c_o;&c_n;&c_g;&c_o;&c_l;</trd>
</drv>

<drv mrk="mongol.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al la <tld/>oj, ilia lingvo, kulturo kaj historio.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_g;&c_o;&c_l;&c_mol;&c_s;&c_k;&c_ib;</trd>
  <trd lng="de">mongolisch</trd>
  <trd lng="en">Mongolian</trd>
  <trd lng="hu">mongol</trd>
  <trd lng="nl">Mongools</trd>
  <trd lng="ru">&c_m;&c_o;&c_n;&c_g;&c_o;&c_l;&c_mol;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="mongol.M0ujo">
  <kap><tld lit="M"/>ujo</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="mongol.M0ujo.teritorio">
    <dif>
      Vasta teritorio en Azio, kie lo&gcirc;as mongoloj.
    </dif>
  </snc>
  <snc mrk="mongol.M0ujo.sxtato">
    <dif>
      <ref tip="super" cel="sxtat.0o">&Scirc;tato</ref> en Azio, limanta al
      <ref tip="vid" cel="rus.R0ujo">Rusujo</ref> kaj al <ref tip="vid" cel="cxin.Cx0ujo">&Ccirc;inujo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_M;&c_a;&c_n;&c_g;&c_o;&c_l;&c_ib;&c_ja;</trd>
  <trd lng="de">Mongolei</trd>
  <trd lng="en">Mongolia</trd>
  <trd lng="ru">&c_M;&c_o;&c_n;&c_g;&c_o;&c_l;&c_i;&c_ja;</trd>
</drv>

<drv mrk="mongol.InternaM0ujo">
  <kap>Interna <tld lit="M"/>ujo</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Parto de Mongolujo, trovi&gcirc;anta en <ref tip="vid" cel="cxin.Cx0ujo">&Ccirc;inujo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_U;&c_n;&c_u;&c_t;&c_r;&c_a;&c_n;&c_a;&c_ja; <ind>&c_M;&c_a;&c_n;&c_g;&c_o;&c_l;&c_ib;&c_ja;</ind></trd>
  <trd lng="ru">&c_V;&c_n;&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_ja;&c_ja; <ind>&c_M;&c_o;&c_n;&c_g;&c_o;&c_l;&c_i;&c_ja;</ind></trd>
</drv>

</art>
