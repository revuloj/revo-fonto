<vortaro>
<art mrk="$Id: mongol.xml,v 1.34 2024/01/26 19:05:53 revo Exp $">
<kap>
  <rad>mongol</rad>/o
</kap>

<drv mrk="mongol.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ano de tradicie nomada <ref tip="lst" lst="voko:popolanoj" cel="popol.0o.etno">popolo</ref>
      de la stepoj inter Siberio kaj la norda &Ccirc;inujo:
      <ekz>
        tiu man&ccirc;ura dinastio estis poste siavice jugita sub la <tld/>oj, kiel
        parto de ties mondimperio, kiu estis preska&ubreve; fari&gcirc;onta vere monda, 
        venkinte en 1241 la aliancajn trupojn de la Sankta Imperio kaj de 
        la poloj &ccirc;e <nom>Liegnitz</nom> en Silezio anta&ubreve; ol ili retiris armeojn je la morto 
        de la Granda &Hcirc;ano en la hejmlando
        <fnt>
          <aut>Sabeo Fu/Yamasaki Seik&ocirc;</aut>
            <vrk><url ref="&Monato;/2001/007025.php">Formortanta lingvo de malplimulto en
            &Ccirc;inio</url></vrk>
            <bib>Monato</bib>
            <lok>2001/01, p. 22</lok>
          </fnt>;
      </ekz>
      <ekz>
        en la 12a kaj 13a jarcentoj la alanoj, la praavoj de nunaj osetoj,
        kreis sian &scirc;taton, kiun ruinigis <tld/>oj, kaj poste la &scirc;tato ne renaski&gcirc;is
        <fnt>
          <bib>Monato</bib>
          <aut>Sergeo Jurji&ccirc;</aut>
          <vrk><url ref="&Monato;/2008/009607.php">&Ccirc;u osetoj ne rajtas sian Kosovon?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ne nur finnoj kaj estonoj partoprenis en la glaciskulptado,
        sed anka&ubreve; &ccirc;inoj el Harbino, <tld/>oj el Ulanbatoro <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2006/009096.php">Glaciskulptado</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_g;&c_o;&c_l;</trd>
  <trd lng="bo">&#xF66;&#xF7C;&#xF42;&#xF0B;&#xF54;&#xF7C;&#xF0B;</trd>
  <trd lng="ca">mongol<klr> (subst.)</klr></trd>
  <trd lng="cs">Mongol</trd>
  <trd lng="de">Mongole</trd>
  <trd lng="en">Mongolian</trd>
  <trd lng="es">mongol</trd>
  <trd lng="fa">&a_m;&a_gh;&a_w;&a_l;</trd>
  <trd lng="fr">Mongole<klr> (subst.)</klr></trd>
  <trd lng="hu">mongol</trd>
  <trd lng="ja">&#x30E2;&#x30F3;&#x30B4;&#x30EB;&#x4EBA;  <pr>&#x30E2;&#x30F3;&#x30B4;&#x30EB;&#x3058;&#x3093;</pr></trd>
  <trd lng="nl">Mongool</trd>
  <trd lng="pl">Mongo&lstroke;</trd>
  <trdgrp lng="pt">
   <trd>mongol</trd>,
   <trd>mogor</trd>,
   <trd>mogol</trd>   
  </trdgrp>
  <trd lng="ru">&c_m;&c_o;&c_n;&c_g;&c_o;&c_l;</trd>
  <trd lng="sk">Mongol</trd>
  <trd lng="uk">&#x43C;&#x43E;&#x43D;&c_g;&#x43E;&#x43B;</trd>
  <trdgrp lng="zh">
    <trd>&#x8499;&#x53E4;&#x65CF; <pr>M&#x11B;ngg&#x1D4;z&#xFA;</pr></trd>,
    <trd>&#x8499;&#x53E4;&#x4EBA; <pr>M&#x11B;ngg&#x1D4;r&#xE9;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mongol.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al la <tld/>oj, ilia lingvo, kulturo kaj historio:
      <ekz>
        la han-genta regno en la suda &Ccirc;inio estis alsorbita al la 
        <tld/>a imperio &dash; tio estis la unua fojo kiam la tuta &Ccirc;inio estis sub fremda regado
        <fnt>
          <aut>Sabeo Fu/Yamasaki Seik&ocirc;</aut>
            <vrk><url ref="&Monato;/2001/007025.php">Formortanta lingvo de malplimulto en
            &Ccirc;inio</url></vrk>
            <bib>Monato</bib>
            <lok>2001/01, p. 22</lok>
          </fnt>;
      </ekz>
      <ekz>
        la <tld/>a lingvo apartenas al la samnoma familio
        <fnt>
          <bib>Monato</bib>
          <aut>Mihail Korotkov</aut>
          <vrk><url ref="&Monato;/2011/010218.php">La origino de la homaj lingvoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_g;&c_o;&c_l;&c_mol;&c_s;&c_k;&c_ib;</trd>
  <trd lng="bo">&#xF66;&#xF7C;&#xF42;&#xF0B;&#xF54;&#xF7C;&#xF60;&#xF72;&#xF0B;&#xF66;&#xF90;&#xF51;&#xF0B;</trd>
  <trd lng="ca">mongol <klr>(adj.)</klr></trd>
  <trd lng="de">mongolisch</trd>
  <trd lng="en">Mongolian</trd>
  <trd lng="es">mongol</trd>
  <trd lng="fa">&a_m;&a_gh;&a_w;&a_l;</trd>
  <trd lng="fr">mongolien</trd>
  <trd lng="hu">mongol</trd>
  <trd lng="ja">&#x30E2;&#x30F3;&#x30B4;&#x30EB;&#x306E;  <pr>&#x3082;&#x3093;&#x3054;&#x308B;&#x306E;</pr></trd>
  <trd lng="nl">Mongools</trd>
  <trd lng="pl">mongolski</trd>
  <trd lng="ru">&c_m;&c_o;&c_n;&c_g;&c_o;&c_l;&c_mol;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="uk">&#x43C;&#x43E;&#x43D;&c_g;&#x43E;&#x43B;&#x44C;&c_s;&#x44C;&#x43A;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x8499;&#x53E4;&#x4EBA; <pr>m&#xE9;ngg&#x1D4;r&#xE9;n</pr></trd>,
    <trd>&#x8499; <pr>m&#x11B;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mongol.M0ujo">
  <kap><tld lit="M"/>ujo</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="mongol.M0ujo.teritorio">
    <dif>
      Vasta teritorio en Azio, kie lo&gcirc;as <tld/>oj.
    </dif>
  </snc>
  <snc mrk="mongol.M0ujo.sxtato">
    <dif>
      <ref tip="super" cel="sxtat.0o">&Scirc;tato</ref> en Azio, limanta al
      <ref tip="vid" cel="rus.R0ujo">Rusujo</ref> kaj al 
      <ref tip="vid" cel="cxin.CX0ujo">&Ccirc;inujo</ref>:
      <ekz>
        <tld lit="M"/>ujo kaj Afganujo restis oficiale sendependaj,
        kvankam <tld lit="M"/>ujo ekzistis kiel sovetunia satelita &scirc;tato
        kaj sovetuniaj trupoj invadis Afganujon en la fino de la 20-a jarcento
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=722">Centra Azio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_M;&c_a;&c_n;&c_g;&c_o;&c_l;&c_ib;&c_ja;</trd>
  <trdgrp lng="ca">
    <trd>Mong&ograve;lia</trd>,
    <trd>Rep&uacute;blica Popular de Mong&ograve;lia</trd>
  </trdgrp>
  <trd lng="de">Mongolei</trd>
  <trd lng="en">Mongolia</trd>
  <trd lng="es">Mongolia</trd>
  <trd lng="fa">&a_m;&a_gh;&a_w;&a_l;&a_s;&a_t;&a_A;&a_n;</trd>
  <trd lng="fr">Mongolie</trd>
  <trd lng="hu">Mong&oacute;lia</trd>
  <trd lng="id">Mongolia</trd>
  <trd lng="nl">Mongoli&euml;</trd>
  <trd lng="pl">Mongolia</trd>
  <trd lng="pt">Mong&oacute;lia</trd>
  <trd lng="ru">&c_M;&c_o;&c_n;&c_g;&c_o;&c_l;&c_i;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#x8499;&#x53E4; <pr>M&#x11B;ngg&#x1D4;</pr></trd>,
    <trd>&#x8499;&#x53E4;&#x56FD; <pr>M&#x11B;ngg&#x1D4;gu&#xF3;</pr></trd>,
    <trd>&#x8499;&#x53E4;&#x570B; <pr>M&#x11B;ngg&#x1D4;gu&#xF3;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mongol.InternaM0ujo">
  <kap>Interna <tld lit="M"/>ujo</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Parto de <tld lit="M"/>ujo, trovi&gcirc;anta en 
      <ref tip="vid" cel="cxin.CX0ujo">&Ccirc;inujo</ref>:
      <ekz>
        Granda &Hcirc;ingan-Montaro <klr>[&#x2026;]</klr> dividas la man&ccirc;uran ebena&jcirc;on
        de nordorienta &Ccirc;inujo oriente <klr>[de]</klr> la <tld lit="M"/>a 
        Altebena&jcirc;o de Interna <tld lit="M"/>ujo okcidente
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=439420">Granda &Hcirc;ingan-Montaro</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_U;&c_n;&c_u;&c_t;&c_r;&c_a;&c_n;&c_a;&c_ja; <ind>&c_M;&c_a;&c_n;&c_g;&c_o;&c_l;&c_ib;&c_ja;</ind></trd>
  <trd lng="ca">Mong&ograve;lia Interior</trd>
  <trd lng="de">Innere <ind>Mongolei</ind></trd>
  <trd lng="en">Inner Mongolia</trd>
  <trd lng="fa">&a_m;&a_gh;&a_w;&a_l;&a_s;&a_t;&a_A;&a_n; &a_d;&a_A;&a_kh;&a_l;&f_y;</trd>
  <trd lng="fr">Mongolie interne</trd>
  <trd lng="hu">Bels&odblac;-<ind>Mong&oacute;lia</ind></trd>
  <trd lng="nl">Binnen-<ind>Mongoli&euml;</ind></trd> 
  <trd lng="pl">Mongolia Wewn&eogonek;trzna</trd>
  <trd lng="ru">&c_V;&c_n;&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_ja;&c_ja; 
<ind>&c_M;&c_o;&c_n;&c_g;&c_o;&c_l;&c_i;&c_ja;</ind></trd>
  <trd lng="zh">&#x6F20;&#x5357; <pr>M&#xF2;n&#xE1;n</pr></trd>
</drv>

