<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tuk</rad>/o
</kap>
<drv mrk="tuk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Peco da teksa&jcirc;o uzata por iu celo:
      <ekz>
        granda nigra <tld/>o volvis la kapon de la virino<fnt><aut>Julian
        Modest</aut>: <vrk>Vetiha</vrk>, <bib>Monato</bib><lok>jaro 2001a,
        numero 2a, p. 26a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">&scaron;&aacute;tek</trd>
  <trd lng="de">Tuch</trd>
  <trd lng="en">cloth</trd>
  <trdgrp lng="fr">
    <trd>pi&egrave;ce de <ind>tissu</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>doek</trd>,
    <trd>kleed</trd>,
    <trd>laken</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_a;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_t;&c_r;&c_ja;&c_p;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="tuk.antaux0o">
  <kap><ofc>*</ofc>anta&ubreve;<tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      <tld lit="T"/>o, kiun oni portas anta&ubreve; si por &scirc;irmi la
      vestojn kontra&ubreve; malpurigo:
      <ekz>
        Dorina rapide demetis la anta&ubreve;<tld/>on, surmetis la pluvmantelon
        kaj eliris eksteren<fnt><aut>Julian Modest</aut>: <vrk>En la ombro
        de la silento</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero 3a,
        p. 14a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Sch&uuml;rze</trd>
  <trd lng="fr">tablier</trd>
  <trd lng="nl">schort</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_d;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_f;&c_a;&c_r;&c_t;&c_u;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="tuk.ban0o">
  <kap>ban<tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Granda sorba <tld/>o uzata por sekigi la korpon post banado a&ubreve;
      lavado.
    </dif>
  </snc>
  <trd lng="de">Badetuch</trd>
  <trdgrp lng="fr">
    <trd>serviette de bain</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>handdoek</trd>,
    <trd>badhanddoek</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_a;&c_n;&c_n;&c_o;&c_je;
  <ind>&c_p;&c_o;&c_l;&c_o;&c_t;&c_je;&c_n;&c_c;&c_je;</ind></trd>
</drv>

<drv mrk="tuk.busx0o">
  <kap>bu&scirc;<tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      <tld lit="T"/>o uzata por purigi la lipojn post man&gcirc;ado
      a&ubreve; trinkado.
    </dif>
  </snc>
  <trd lng="de">Serviette</trd>
  <trdgrp lng="fr">
    <trd>serviette de table</trd>
  </trdgrp>
  <trd lng="nl">servet</trd>
  <trd lng="ru">&c_s;&c_a;&c_l;&c_f;&c_je;&c_t;&c_k;&c_a;</trd>
</drv>

<drv mrk="tuk.kap0o">
  <kap>kap<tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Hararkovra tuko uzata kiel <ref tip="super" cel="vest.kap0o">kapvesto</ref>:
      <ekz>
        Vetiha forprenis la kap<tld/>on kaj &jcirc;etis &gcirc;in sur la
        plankon<fnt><aut>Julian Modest</aut>: <vrk>Vetiha</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 2a, p. 26a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>fichu<klr> (de t&ecirc;te)</klr></trd>,
    <trd>foulard<klr> (de t&ecirc;te)</klr></trd>,
    <trd>tchador</trd>
  </trdgrp>
</drv>

<drv mrk="tuk.kol0o">
  <kap>kol<tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Gea <ref tip="super" cel="vest.sur0o">survesto</ref>
      el tukstrio metita a&ubreve; diversmaniere volvita &ccirc;irka&ubreve;
      la kolo,
      uzata orname kaj kontra&ubreve; malvarmo.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>cache-nez</trd>,
    <trd>&eacute;charpe</trd>,
    <trd>foulard</trd>
  </trdgrp>
</drv>

<drv mrk="tuk.lit0o">
  <kap>lit<tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Granda tola a&ubreve; kotona tuko, per kiu oni
      kovras litan matracon a&ubreve; kiun oni surtiras super sin
      por dormi.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>drap de lit</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_t;&c_y;&c_n;&c_ja;</trd>
</drv>

<drv mrk="tuk.morto0o">
  <kap>morto<tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Tola&jcirc;o, per kiu oni &ccirc;irka&ubreve;volvas la korpon
      de mortino
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>linceul</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_a;&c_v;&c_a;&c_n;</trd>
</drv>

<drv mrk="tuk.naz0o">
  <kap>naz<tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <ref tip="dif" cel="tuk.posx0o">po&scirc;<tld/>o</ref>
  </snc>
  <trd lng="ru">&c_n;&c_o;&c_s;&c_o;&c_v;&c_o;&c_j;
  &c_p;&c_l;&c_a;&c_t;&c_o;&c_k;</trd>
</drv>

<drv mrk="tuk.polvo0o">
  <kap>polvo<tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      <tld lit="T"/>o, uzata seka por forigi polvon de horizontalaj
      surfacoj.
    </dif>
  </snc>
  <trd lng="de">Staubtuch</trd>
  <trdgrp lng="fr">
    <trd>chiffon &agrave; poussi&egrave;re</trd>
  </trdgrp>
  <trd lng="nl">stofdoek</trd>
  <trd lng="ru">&c_t;&c_r;&c_ja;&c_p;&c_k;&c_a; &c_d;&c_l;&c_ja;
  &c_p;&c_y;&c_l;&c_i;</trd>
</drv>

<drv mrk="tuk.posx0o">
  <kap>po&scirc;<tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Eta tuko, kiun oni elpo&scirc;igas por vi&scirc;i &scirc;viton
      a&ubreve; nazan mukon.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="tuk.naz0o">naz<tld/>o</ref>
  </refgrp>
  <trdgrp lng="fr">
    <trd>mouchoir</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_o;&c_s;&c_o;&c_v;&c_o;&c_j;
  &c_p;&c_l;&c_a;&c_t;&c_o;&c_k;</trd>
</drv>

<drv mrk="tuk.sekig0o">
  <kap>sekig<tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      <tld lit="T"/>o, uzata por sekigi telerojn post purigado.
    </dif>
  </snc>
  <trd lng="de">Geschirrtuch</trd>
  <trd lng="en">tea towel</trd>
  <trd lng="fr">torchon &agrave; vaisselle</trd>
  <trd lng="nl">theedoek</trd>
</drv>

<drv mrk="tuk.visx0o">
  <kap>vi&scirc;<tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      <tld lit="T"/>o ofte &ccirc;ifona, uzata por vi&scirc;i a&jcirc;ojn.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>chiffon</trd>,
    <trd>serpill&egrave;re</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_o;&c_t;&c_je;&c_n;&c_c;&c_je;</trd>
</drv>
</art>
