<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tuk</rad>/o
</kap>
<drv mrk="tuk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Peco da teksa&jcirc;o uzata por iu celo:
      <ekz>
        bu&scirc;<tld/>o;
      </ekz>
      <ekz>
        <ind>naz<tld/>o</ind>;
        <trd lng="ru">&c_n;&c_o;&c_s;&c_o;&c_v;&c_o;&c_j; &c_p;&c_l;&c_a;&c_t;&c_o;&c_k;</trd>
      </ekz>
      <ekz>
        <ind>lit<tld/>o</ind>;
        <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_t;&c_y;&c_n;&c_ja;</trd>
      </ekz>
      <ekz>
        <ind>vi&scirc;<tld/>o</ind>;
        <trd lng="ru">&c_p;&c_o;&c_l;&c_o;&c_t;&c_je;&c_n;&c_c;&c_je;</trd>
      </ekz>
      <ekz>
        <ind>morto<tld/>o</ind>.
        <trd lng="ru">&c_s;&c_a;&c_v;&c_a;&c_n;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">&scaron;&aacute;tek</trd>
  <trd lng="de">Tuch</trd>
  <trd lng="en">cloth</trd>
  <trdgrp lng="nl">
    <trd>doek</trd>,
    <trd>kleed</trd>,
    <trd>laken</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_a;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_t;&c_r;&c_ja;&c_p;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="tuk.antaux0o">
  <kap><ofc>*</ofc>anta&ubreve;<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="T"/>o, kiun oni portas anta&ubreve; si por &scirc;irmi la vestojn
      kontra&ubreve; malpurigo.
    </dif>
  </snc>
  <trd lng="de">Sch&uuml;rze</trd>
  <trd lng="fr">tablier</trd>
  <trd lng="nl">schort</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_d;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_f;&c_a;&c_r;&c_t;&c_u;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="tuk.ban0o">
  <kap>ban<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="T"/>o uzata por sekigi sin post banado a&ubreve; lavado.
    </dif>
  </snc>
  <trd lng="de">Badetuch</trd>
  <trdgrp lng="nl">
    <trd>handdoek</trd>,
    <trd>badhanddoek</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_a;&c_n;&c_n;&c_o;&c_je; <ind>&c_p;&c_o;&c_l;&c_o;&c_t;&c_je;&c_n;&c_c;&c_je;</ind></trd>
</drv>

<drv mrk="tuk.busx0o">
  <kap>bu&scirc;<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="T"/>o uzata por purigi la lipojn post man&gcirc;ado
      a&ubreve; trinkado.
    </dif>
  </snc>
  <trd lng="de">Serviette</trd>
  <trd lng="nl">servet</trd>
  <trd lng="ru">&c_s;&c_a;&c_l;&c_f;&c_je;&c_t;&c_k;&c_a;</trd>
</drv>

<drv mrk="tuk.polvo0o">
  <kap>polvo<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="T"/>o uzata seka por forigi polvon de horizontalaj
      surfacoj.
    </dif>
  </snc>
  <trd lng="de">Staubtuch</trd>
  <trd lng="nl">stofdoek</trd>
  <trd lng="ru">&c_t;&c_r;&c_ja;&c_p;&c_k;&c_a; &c_d;&c_l;&c_ja; &c_p;&c_y;&c_l;&c_i;</trd>
</drv>

<drv mrk="tuk.sekig0o">
  <kap>sekig<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="T"/>o uzata por sekigi telerojn post purigado.
    </dif>
  </snc>
  <trd lng="de">Geschirrtuch</trd>
  <trd lng="en">tea towel</trd>
  <trd lng="fr">torchon</trd>
  <trd lng="nl">theedoek</trd>
</drv>
</art>
