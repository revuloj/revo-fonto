<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bulvar.xml,v 1.26 2025/04/17 08:37:41 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>bulvard</rad>/o
<fnt><bib>JED</bib></fnt></kap>

<drv mrk="bulvar.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TRA</uzo>
  <snc>
    <dif>
      &Ccirc;irka&ubreve;rempara promenejo interne de urbo.
    </dif>
  </snc>
  <snc>
    <dif>
      Lar&gcirc;a <ref tip="super" cel="strat.0o">strato</ref>
      borderita per arboj, kutime surloke de iamaj urbomuroj:
      <ekz>
        proklamas la ri&ccirc;on de l' urba kulturo butikoj, <tld/>oj
        lumfeste kronitaj
        <fnt>
          <aut>Krys Ungar</aut>,
          <vrk>Meznokto metropola</vrk>,
          <lok>p. 97</lok>
        </fnt>;
      </ekz>
      <ekz>
        la granda <tld/>o trairanta la &ccirc;efurbon
        <fnt><aut>Bardhyl
        Selimi</aut>: <vrk>Tirana politiko renversita</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 5a, p. 17a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ale.0o">aleo</ref>,
      <ref cel="avenu.0o">avenuo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_l;&c_mol;&c_v;&c_a;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>boulevard</trd>,
    <trd>bulv&aacute;r</trd>,
    <trd>hlavn&iacute; ulice</trd>
  </trdgrp>
  <trd lng="de">Boulevard<klr tip="amb"> (Prachtstra&szlig;e)</klr>
  </trd>
  <trd lng="en">boulevard</trd>
  <trd lng="es">bulevar</trd>
  <trd lng="fa">&a_b;&a_l;&a_w;&a_A;&a_r;</trd>
  <trd lng="fr">boulevard</trd>
  <trd lng="he">&shin;&dalet;&resh;&he;</trd>
  <trdgrp lng="hu">
    <trd>k&ouml;r&uacute;t</trd>,
    <trd>sug&aacute;r&uacute;t</trd>,
    <trd>bulv&aacute;r</trd>
  </trdgrp>
  <trd lng="id">bulevar</trd>
  <trdgrp lng="ja">
    <trd>&#22823;&#36890;&#12426;  <pr>&#12362;&#12362;&#12393;&#12362;&#12426;</pr></trd>,
    <trd>&#12502;&#12540;&#12523;&#12496;&#12540;&#12523;</trd>
  </trdgrp>
  <trd lng="nl">boulevard</trd>
  <trdgrp lng="pl">
    <trd>bulwar</trd>,
    <trd>promenada</trd>
  </trdgrp>
  <trd lng="ro">bulevard</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_u;&c_l;&c_mol;&c_v;&c_a;&c_r;</trd>,
    <trd>&c_a;&c_l;&c_l;&c_je;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bulv&aacute;r</trd>,
    <trd>trieda <klr>(&scaron;irok&aacute; ulica)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_l;&c_mol;&c_v;&c_a;&c_r;</trd>,
    <trd>&c_a;&c_l;&c_je;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="bulvar.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Rilata al <tld/>o, celata al surstrata publiko:
      <ekz>
        <tld/>a teatro;
      </ekz>
      <ekz>
        <tld/>aj gazetoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2005/008687.php"
              >Postmilite</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_l;&c_mol;&c_v;&c_a;&c_r;&c_n;&c_y;</trd>
  <trd lng="de">Boulevard-</trd>
</drv>

<drv mrk="bulvar.0ismo">
  <kap>
    <tld/>ismo
  </kap>
  <snc>
    <uzo tip="fak">POL</uzo>
    <dif>
      Malserioza <ref tip="super" cel="jxurna.0ismo">&jcirc;urnalismo</ref>:
      <ekz>
        la gazetaro adapti&gcirc;is kaj &scirc;an&gcirc;i&gcirc;is pro la televido (kun tendenco al <tld/>ismo)
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan MAUL</aut>
          <vrk><url ref="&Monato;/2004/008241.html"
              >Mallegemo</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_l;&c_mol;&c_v;&c_a;&c_r;&c_n;&c_a;&c_ja; &c_zh;&c_u;&c_r;&c_n;&c_a;&c_l;&c_ib;&c_s;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="de">Boulevardjorunalismus</trd>
</drv>

</art>
