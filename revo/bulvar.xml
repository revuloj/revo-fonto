<kap>
  <ofc>1</ofc>
  <rad>bulvard</rad>/o
</kap>

<drv mrk="bulvar.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TRA</uzo>
  <snc>
    <dif>
      &Ccirc;irka&ubreve;rempara promenejo interne de urbo.
    </dif>
  </snc>
  <snc>
    <dif>
      Lar&gcirc;a <ref tip="super" cel="strat.0o">strato</ref>
      borderita per arboj:
      <ekz>
        proklamas la ri&ccirc;on de l' urba kulturo butikoj, <tld/>oj
        lumfeste kronitaj
        <fnt>
          <aut>Krys Ungar</aut>,
          <vrk>Meznokto metropola</vrk>,
          <lok>p. 97</lok>
        </fnt>;
      </ekz>
      <ekz>
        la granda <tld/>o trairanta la &ccirc;efurbon
        <fnt><aut>Bardhyl
        Selimi</aut>: <vrk>Tirana politiko renversita</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 5a, p. 17a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ale.0o">aleo</ref>,
      <ref cel="avenu.0o">avenuo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_l;&c_mol;&c_v;&c_a;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>boulevard</trd>,
    <trd>bulv&#xE1;r</trd>,
    <trd>hlavn&#xED; ulice</trd>
  </trdgrp>
  <trd lng="de">Boulevard<klr tip="amb"> (Prachtstra&szlig;e)</klr>
  </trd>
  <trd lng="en">boulevard</trd>
  <trd lng="es">bulevar</trd>
  <trd lng="fa">&a_b;&a_l;&a_w;&a_A;&a_r;</trd>
  <trd lng="fr">boulevard</trd>
  <trd lng="he">&#x5E9;&#x5D3;&#x5E8;&#x5D4;</trd>
  <trdgrp lng="hu">
    <trd>k&ouml;r&uacute;t</trd>,
    <trd>sug&aacute;r&uacute;t</trd>,
    <trd>bulv&aacute;r</trd>
  </trdgrp>
  <trd lng="id">bulevar</trd>
  <trdgrp lng="ja">
    <trd>&#x5927;&#x901A;&#x308A;  <pr>&#x304A;&#x304A;&#x3069;&#x304A;&#x308A;</pr></trd>,
    <trd>&#x30D6;&#x30FC;&#x30EB;&#x30D0;&#x30FC;&#x30EB;</trd>
  </trdgrp>
  <trd lng="nl">boulevard</trd>
  <trdgrp lng="pl">
    <trd>bulwar</trd>,
    <trd>promenada</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_u;&c_l;&c_mol;&c_v;&c_a;&c_r;</trd>,
    <trd>&c_a;&c_l;&c_l;&c_je;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bulv&#xE1;r</trd>,
    <trd>trieda <klr>(&#x161;irok&#xE1; ulica)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x431;&#x443;&#x43B;&#x44C;&#x432;&#x430;&#x440;</trd>,
    <trd>&#x430;&#x43B;&#x435;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
