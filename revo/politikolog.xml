<drv mrk="politikolog.0o">
  <kap><tld/>o, <var><kap><tld var="t"/>o</kap></var></kap>
  <snc mrk="politikolog.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="vid" cel="politikologi.0o.POL">Politikologi</ref>isto,
      specialisto pri analizado de politiko:
      <ekz>
        miaj pensoj pri demokratio multe influi&gcirc;is de diversaj a&ubreve;toroj,
        mi nepre citu unu: la <tld/>on <nom>Robert Dahl</nom>
        <fnt>
          <aut>I. De Coster</aut>,
          <vrk><url ref="http://www.helsinki.fi/~jslindst/raumo-id.html">
          Esperanto-komunumo kaj la civila socio</url></vrk>,
          <lok>en: Kultura Esperanto-Festivalo, Helsinki, 2000</lok>
        </fnt>;
      </ekz>    
      <ekz>
        <tld var="t"/>oj <klr>[&#x2026;]</klr> kritikis juran formalismon    
        <fnt>
          <aut>A. Lewanderska-Quednau</aut>,
          <vrk><url ref="http://w3.forst.wzw.tum.de/~quednau/AISKURSOJ/ALQ/politologio.pdf">
          Politologio - historia skizo, transformi&gcirc;oj</url></vrk>,
          <lok>AIS, 2000</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ib;&c_t;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="bg">&c_p;&c_o;&c_l;&c_i;&c_t;&c_o;&c_l;&c_o;&c_g;</trd>
