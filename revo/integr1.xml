  </snc>
  <trd lng="be">&c_ib;&c_n;&c_t;&c_e;&c_g;&c_r;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="bg">&c_i;&c_n;&c_t;&c_je;&c_g;&c_r;&c_i;&c_r;&c_a;&c_n;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>integrace</trd>,
    <trd>integrov&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Integration</trd>
  <trd lng="es">integraci&oacute;n</trd>
  <trd lng="hu">integr&aacute;l&aacute;s</trd>
  <trd lng="ru">&c_i;&c_n;&c_t;&c_je;&c_g;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">integrovanie</trd>
  <trd lng="uk">&#x456;&#x43D;&#x442;&#x435;&#x491;&#x440;&#x443;&#x432;&#x430;&#x43D;&#x43D;&#x44F;</trd>
</drv>

<drv mrk="integr1.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 21</lok></fnt>
  <snc>
    <dif>
      <ref tip="dif" cel="integr2.0o">Integralo</ref>:
      <ekz>
        la <tld/>a&jcirc;o de la produto de la du funkcioj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=240859">Kunfalda&jcirc;o</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_t;&c_e;&c_g;&c_r;&c_a;&c_l;</trd>
  <trd lng="bg">&c_i;&c_n;&c_t;&c_je;&c_g;&c_r;&c_a;&c_l;</trd>
  <trd lng="de">Integral</trd>
  <trd lng="es">integral</trd>
  <trd lng="hu">integr&aacute;l</trd>
  <trd lng="ru">&c_i;&c_n;&c_t;&c_je;&c_g;&c_r;&c_a;&c_l;</trd>
  <trd lng="uk">&#x456;&#x43D;&#x442;&#x435;&#x491;&#x440;&#x430;&#x43B;</trd>
</drv>

<drv mrk="integr1.0ato">
  <kap><tld/>ato</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <ref tip="dif" cel="integr2.0ato">integralato.</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_y;&c_n;&c_t;&c_e;&c_g;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja;
    <ind>&c_f;&c_u;&c_n;&c_k;&c_c;&c_y;&c_ja;</ind></trd>,
    <trd>&c_p;&c_a;&c_d;&c_y;&c_n;&c_t;&c_e;&c_g;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;
    <ind>&c_v;&c_y;&c_r;&c_a;&c_z;</ind></trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_o;&c_d;&c_i;&c_n;&c_t;&c_je;&c_g;&c_r;&c_a;&c_l;&c_je;&c_n; &c_i;&c_z;&c_r;&c_a;&c_z;</trd>
  <trdgrp lng="cs">
    <trd>integrand</trd>,
    <trd>integrovan&yacute; v&yacute;raz</trd>
  </trdgrp>
  <trd lng="de">Integrand</trd>
  <trd lng="es">integrando <klr>(funci&oacute;n a integrar)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_y;&c_n;&c_t;&c_je;&c_g;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_je;
    &c_v;&c_y;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_y;&c_n;&c_t;&c_je;&c_g;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja;
    &c_f;&c_u;&c_n;&c_k;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">integrovan&yacute; v&yacute;raz</trd>
  <trd lng="uk">&#x43F;&#x456;&#x434;&#x456;&#x43D;&#x442;&#x435;&#x491;&#x440;&#x430;&#x43B;&#x44C;&#x43D;&#x430; &#x444;&#x443;&#x43D;&#x43A;&#x446;&#x456;&#x44F;</trd>
</drv>

<drv mrk="integr1.0ebla">
  <kap><tld/>ebla</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="dif" cel="integr2.0ebla">Integralebla</ref>:
      <ekz>
        kvadrate <tld/>ebla funkcio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=418301">Vico de Fourier</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_i;&c_n;&c_t;&c_je;&c_g;&c_r;&c_i;&c_r;&c_u;&c_je;&c_m;</trd>
  <trd lng="de">integrierbar</trd>
  <trd lng="es">integrable</trd>
  <trd lng="hu">integr&aacute;lhat&oacute;</trd>
  <trd lng="ru">&c_i;&c_n;&c_t;&c_je;&c_g;&c_r;&c_i;&c_r;&c_u;&c_je;&c_m;&c_y;&c_j;</trd>
  <trd lng="uk">&#x456;&#x43D;&#x442;&#x435;&#x491;&#x440;&#x43E;&#x432;&#x43D;&#x438;&#x439;</trd>
</drv>

<drv mrk="integr1.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 21</lok></fnt>
  <snc>
    <ref tip="dif" cel="integr2.0signo">integralsigno.</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_n;&c_t;&c_e;&c_g;&c_r;&c_a;&c_l; <klr tip="amb">(&c_z;&c_n;&c_a;&c_k;)</klr></trd>,
    <trd>&c_z;&c_n;&c_a;&c_k;
    &c_ib;&c_n;&c_t;&c_e;&c_g;&c_r;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ja;</trd>
  </trdgrp>
  <trd lng="bg">&c_i;&c_n;&c_t;&c_je;&c_g;&c_r;&c_a;&c_l;&c_je;&c_n; &c_z;&c_n;&c_a;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>integr&aacute;tor</trd>,
    <trd>znak integr&aacute;lu</trd>
  </trdgrp>
  <trd lng="de">Integralzeichen</trd>
  <trd lng="es">signo de integral</trd>
  <trd lng="hu">integr&aacute;ljel</trd>
  <trd lng="ru">&c_z;&c_n;&c_a;&c_k;
  <ind>&c_i;&c_n;&c_t;&c_je;&c_g;&c_r;&c_a;&c_l;&c_a;</ind></trd>
  <trd lng="sk">znak integr&aacute;lu</trd>
