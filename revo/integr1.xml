<art mrk="$Id: integr1.xml,v 1.21 2022/03/21 15:27:49 revo Exp $">
<kap>
  <rad>integr</rad>/i
</kap>
<drv mrk="integr1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 22</lok></fnt>
  <snc>
    <ref tip="dif" cel="integr2.0i">integrali</ref>
    <rim>
      En la &ccirc;i-supra fontindiko <nom>Bricard</nom> parolas nur pri
      <ctl><tld/>i diferencialan ekvacion</ctl>, sed certe la
      senco <ctl><tld/>i funkcion</ctl> ne estis al li fremda.
    </rim>
  </snc>
  <ref tip="hom" cel="integr.0i">integri (esti tuteca)</ref>
  <trd lng="be">&c_ib;&c_n;&c_t;&c_e;&c_g;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_i;&c_n;&c_t;&c_je;&c_g;&c_r;&c_i;&c_r;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>integrovat</trd>,
    <trd>po&ccaron;&iacute;tat integr&aacute;l</trd>,
    <trd>slu&ccaron;ovat</trd>
  </trdgrp>
  <trd lng="de">integrieren</trd>
  <trd lng="es">integrar <klr tip="amb">(mat.)</klr></trd>
  <trd lng="hu">integr&aacute;l</trd>
  <trd lng="pt">integrar <klr tip="amb">(mat.)</klr></trd>
  <trd lng="ru">&c_i;&c_n;&c_t;&c_je;&c_g;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>integrova&tcaron;</trd>,
    <trd>zlu&ccaron;ova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x454;&#x434;&#x43D;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x456;&#x43D;&#x442;&#x435;&#x491;&#x440;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43C;&#x430;&#x442;. &#x456;&#x43D;&#x442;&#x435;&#x491;&#x440;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="integr1.0ado">
  <kap><tld/>ado</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 21</lok></fnt>
