<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>la&ubreve;r</rad>/o
    </kap>

<drv mrk="lauxr.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="lauxr.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="lauxrac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Laurus</trd>), kreskanta en mediteranea
      regiono, &ccirc;iamverda arbo kun ledaj folioj kaj blunigraj 
      beroj, en la antikva tempo uzata por kroni venkintojn:
      <ekz>
        floranta oleandro kaj fre&scirc;aj <tld/>bran&ccirc;oj, alportitaj el
        malanta&ubreve; la montoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en la krono, kiu ornamas lian gajan kapon, elkreskas nova folio
        de <tld/>o
        (vd <ref tip="vid" cel="lauxr.0okrono">la&ubreve;rokrono</ref>)
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    Kp <ref tip="hom" cel="lauxr1.0o">La&ubreve;ro</ref> (virina nomo)
  </rim>
  <trdgrp lng="br">
    <trd>loreenn</trd>,
    <trd>gwezenn-lore</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>g&egrave;nere Laurus</trd>,
    <trd>llorer</trd>
  </trdgrp>
  <trd lng="de">Lorbeerbaum</trd>
  <trdgrp lng="en">
    <trd>laurel</trd>,
    <trd>bay tree</trd>
  </trdgrp>
  <trd lng="es">laurel</trd>
  <trd lng="fr">laurier</trd>
  <trd lng="hu">bab&eacute;rfa</trd>
  <trd lng="nl">laurier</trd>
  <trd lng="pl">laur</trd>
  <trd lng="pt">louro<klr tip="amb"> (subst., bot.)</klr></trd>
  <trd lng="be">&c_l;&c_a;&c_w;&c_r;</trd>
  <trd lng="ru">&c_l;&c_a;&c_v;&c_r;</trd>
</drv>

<drv mrk="lauxr.0oj">
  <kap>
    <tld/>oj
  </kap>
  <snc mrk="lauxr.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Plurnombro de &leftquot;<tld/>o&rightquot;:
      <ekz>
        en Hispanujo, kie inter oran&gcirc;oj kaj <tld/>oj estas
        &ccirc;irka&ubreve;blovataj 
        de varmaj ventetoj la oraj Ma&ubreve;raj kupoloj, kie sonas kantado kaj
        kastanjetoj
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="br">
      <trd>lore</trd>,
      <trd>gwez-lore</trd>
    </trdgrp>
    <trd lng="nl">lauweren</trd>
    <trd lng="pl">laury</trd>
  </snc>
  <snc mrk="lauxr.0oj.gloro">
    <uzo tip="stl">FIG</uzo>
    <dif>
     <ref tip="dif" cel="lauxr.0okrono"><tld lit="L"/>okrono</ref>:
      <ekz>
        la <tld/>oj de la gloro;
      </ekz><ekz>
        <tld/>e kroni triumfinton;
      </ekz><ekz>
        rikolti <tld/>ojn <klr>(multe sukcesi)</klr>;
      </ekz>
      <ekz>
        kelke da pa&scirc;oj plue trovi&gcirc;as la tomba monumento de Alfieri,
        ornamita per <tld/>oj, liro kaj maskoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi bezonas nur iom ripozi &mdash; <klr>(ironie)</klr> sur viaj la&ubreve;roj,
        jes! 
        <klr>(ne finu strebi post ricevi meritojn)</klr>
        <fnt>
          <bib>FK</bib>
        </fnt>
      </ekz>
    </dif>
    <trd lng="de">Lorbeeren <klr tip="ind">(Sieg, Ruhm)</klr></trd>
    <trd lng="en">laurels</trd>
    <trd lng="hu">bab&eacute;r<klr tip="ind">(dics&odblac;s&eacute;g)</klr></trd>
    <trd lng="nl">lauwerkrans</trd>
    <trd lng="pl">laury</trd>
    <trdgrp lng="pt">
      <trd>laureal</trd>,
      <trd>laurel</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">lauriers</trd>
  <trd lng="ru">&c_l;&c_a;&c_v;&c_r;&c_y;</trd>
</drv>

<drv mrk="lauxr.0okrono">
  <kap>
    <tld/>okrono
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="kron.0o.plektita">Krono<sncref/></ref>
      el <tld/>aj bran&ccirc;etoj, kutima honorsigno &ccirc;e la
      antikvaj
      grekoj kaj romianoj; metafore, simbolo de venko, sukceso,
      merito:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kion ajn fifamigan oni alie povis diri pri
        <ctl>Jorike</ctl>, pro unu afero &gcirc;i meritis
        <tld/>okronon: &gcirc;i estis eminenta instrumajstro
        <fnt>
          <aut>B. Traven, tr. Hans-Georg Kaiser</aut>:
          <vrk>La Mortula &scirc;ipo</vrk>.
          <lok>
          <url
          ref="http://home.t-online.de/home/kaiser-altenburg/u/B.Traven/m31-35.htm">
          &Ccirc;ap. 31a</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la blazon&scirc;ildon <klr>[de Argentino]</klr> &ccirc;irka&ubreve;as <tld/>okrono,
        
        kiu reprezentu la armean venkon en la batalo pri &scirc;tata
        sendependeco
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=388271">Blazono
          de Argentino</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">kurunenn lore</trd>
  <trdgrp lng="ca">
    <trd>corona de llorer</trd>,
    <trd>laur&egrave;ola</trd>
  </trdgrp>
  <trd lng="de">Loorbeerkranz</trd>
  <trd lng="nl">lauwerkrans</trd>
  <trd lng="pl">wieniec <ind>laurowy</ind></trd>
  <trd lng="ru">&c_l;&c_a;&c_v;&c_r;&c_o;&c_v;&c_y;&c_j;
  &c_v;&c_je;&c_n;&c_o;&c_k;</trd>
</drv>

  </art>
