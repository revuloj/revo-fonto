<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>la&ubreve;r</rad>/o
</kap>
<drv mrk="lauxr.0o">
  <kap><tld/>o</kap>
  <snc mrk="lauxr.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="lauxrac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Laurus</trd>), kreskanta en mediteranea regiono,
      &ccirc;iamverda arbo kun ledaj folioj
      kaj blunigraj beroj, en la antikva tempo uzata por kroni venkintojn.
    </dif>
    <trd lng="de">Lorbeerbaum</trd>
    <trdgrp lng="en">
      <trd>laurel</trd>,
      <trd>bay tree</trd>
    </trdgrp>
    <trd lng="nl">laurier</trd>
</snc>
  <snc mrk="lauxr.0o.gloro">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Simbolo de venko, sukceso:
      <ekz>
        la <tld/>oj de la gloro;
      </ekz>
      <ekz>
        <tld/>e kroni triumfinton;
      </ekz>
      <ekz>
        rikolti <tld/>ojn <klr>(multe sukcesi)</klr>.
      </ekz>
    </dif>
    <trd lng="de">Lorbeeren <klr tip="ind">(Sieg, Ruhm)</klr></trd>
    <trd lng="en">laurels</trd>
    <trd lng="nl">lauwerkrans</trd>
</snc>
</drv>

</art>
