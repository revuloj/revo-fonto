<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>la&ubreve;r</rad>/o
</kap>
<drv mrk="lauxr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="lauxr.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Laurus</trd>), kreskanta en Mediteranea regiono,
      &ccirc;iamverda arbo kun ledaj folioj
      kaj blunigraj beroj, en la antikva tempo uzata por kroni venkintojn.
    </dif>
    <trd lng="de">Lorbeerbaum</trd>
  </snc>
  <snc>
    <uzo tip="klr">(f)</uzo>
    <dif>
      Simbolo de venko, sukceso:
      <ekz>
        la <tld/>oj de la gloro;
      </ekz>
      <ekz>
        <tld/>e kroni triumfinton;
      </ekz>
      <ekz>
        rikolti <tld/>ojn <klr>(multe sukcesi)</klr>.
      </ekz>
    </dif>
    <trd lng="de">Lorbeeren <klr tip="ind">(Sieg, Ruhm)</klr></trd>
  </snc>
</drv>
<drv mrk="lauxr.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj (<trd lng="la">Lauraceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Lorbeergew&auml;chse</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
