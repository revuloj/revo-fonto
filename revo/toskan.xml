<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: toskan.xml,v 1.9 2023/10/21 14:37:42 revo Exp $">
<kap>
  <rad>toskan</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="toskan.0o">
  <kap><tld/>o</kap>
  <snc mrk="toskan.0o.logxanto">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Lo&gcirc;anto de <ref tip="malprt" cel="toskan.0ujo.GEOG">Toskanujo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_s;&c_k;&c_a;&c_n;&c_je;&c_c;</trd>
  <trd lng="ca">tosc&agrave;<klr> (subst.)</klr></trd>
  <trd lng="fr">toscan<klr> (subst. personne)</klr></trd>
  <trd lng="it">toscano</trd>
  <trd lng="en">Tuskanian</trd>
  <trd lng="ja">&#x30C8;&#x30B9;&#x30AB;&#x30CA;&#x4EBA;  <pr>&#x30C8;&#x30B9;&#x30AB;&#x30CA;&#x3058;&#x3093;</pr></trd>
  <trd lng="pl">Toska&nacute;czyk</trd>
  <trd lng="uk">&#x442;&#x43E;&#x441;&#x43A;&#x430;&#x43D;&#x435;&#x446;&#x44C;</trd>
</drv>

<drv mrk="toskan.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>oj a&ubreve; al <tld lit="T"/>ujo.
    </dif>
    <ekz>
      <tld/>a literaturo;
    </ekz>
    <ekz>
      <tld/>a arto.
    </ekz>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_s;&c_k;&c_a;&c_n;&c_s;&c_k;&c_ib;</trd>
  <trd lng="ca">tosc&agrave;<klr> (adj.)</klr></trd>
  <trd lng="fr">toscan<klr> (adj.)</klr></trd>
  <trd lng="it">toscano <klr>(adj)</klr></trd>
  <trd lng="en">Tuskanian</trd>
  <trd lng="pl">toska&nacute;ski</trd>
  <trd lng="uk">&#x442;&#x43E;&#x441;&#x43A;&#x430;&#x43D;&#x441;&#x44C;&#x43A;&#x438;&#x439;</trd>
</drv>

<drv mrk="toskan.0ujo">
  <kap><tld lit="T"/>ujo</kap>
  <snc mrk="toskan.0ujo.GEOG">	
    <uzo tip="fak">GEOG</uzo>
    <dif>
     Provinco en centra <ref tip="malprt" cel="ital.I0ujo">Italujo</ref>
     kun &ccirc;efurbo Florenco.
    </dif>
  </snc>
  <trd lng="be">&c_T;&c_a;&c_s;&c_k;&c_a;&c_n;&c_a;</trd>
  <trd lng="ca">la <ind>Toscana</ind></trd>
  <trd lng="fr">Toscane</trd>
  <trd lng="it">Toscana</trd>
  <trd lng="en">Tuscany</trd>
  <trd lng="pl">Toskania</trd>
</drv>
</art>

