<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>toskan</rad>/o
</kap>
<drv mrk="toskan.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="toskan.0o.logxanto">
    <dif>
Lo&gcirc;anto de 
<ref tip="malprt" cel="toskan.Oio">Toskanio</ref>.
    </dif>
  </snc>
  <trd lng="it">toscano</trd>
  <trd lng="en">Tuskanian</trd>
</drv>

<drv mrk="toskan.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
Rilatanta al la <tld lit="T"/>oj a&ubreve; al <tld lit="T"/>io.
    </dif>
    <ekz>
<tld/>a literaturo; <tld/> arto.
      </ekz>
  </snc>
  <trd lng="it">toscano <klr>(adj)</klr></trd>
  <trd lng="en">Tuskanian</trd>
</drv>

<drv mrk="toskan.0io">
  <kap><tld lit="T"/>io</kap>
  <uzo tip="fak">GEOG</uzo>
<fnt>
<bib>Viki</bib>
<url ref="http://eo.wikipedia.org/wiki/Toskanio">
Toskanio</url></fnt>
  <snc>
    <dif>
     Regiono de centra 
<ref tip="malprt" cel="ital.I0ujo">Italujo</ref>. &Gcirc;ia &ccirc;efurbo 
estas Florenco.
    </dif>
  </snc>
  <trd lng="it">Toscana</trd>
  <trd lng="en">Tuscany</trd>
</drv>

</art>

