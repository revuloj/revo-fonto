<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <rad>kinkon</rad>/o
</kap>
<drv mrk="kinkon.0o">
   <kap><tld/>o</kap>
   <snc mrk="kinkon.0o.BOT">
     <uzo tip="fak">BOT</uzo>
     <dif>
       G. de rubiacoj, el Peruo, kiu liveras amaran kaj kontra&ubreve;febran
       &scirc;elon.
     </dif>
     <trd lng="be">&c_h;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_je; &c_d;&c_r;&c_e;&c_v;&c_a;</trd>
     <trdgrp lng="de">
       <trd>Chinabaum</trd>,
       <trd>Chinarindenbaum</trd>
     </trdgrp>
     <trdgrp lng="en">
       <trd>cinchona</trd>,
       <trd>cinchona tree</trd>
     </trdgrp>
     <trd lng="fr">quinquina<klr tip="ind"> (bot.)</klr></trd>
     <trd lng="hu">k&iacute;nafa</trd>
     <trd lng="nl">kinaboom</trd>
     <trd lng="ru">&c_h;&c_i;&c_n;&c_n;&c_o;&c_je;
     &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
   </snc>
   <snc mrk="kinkon.0o.KEM">
     <uzo tip="fak">KEM</uzo>
     <dif>
       La &scirc;elo de <tld/>o:
       <ekz>
         la <tld/>o estas ekspluatata pro siaj terapiaj proprecoj.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="kinin.0o">kinino</ref>,
       <ref cel="kinkon1.0o">kinkonino</ref>.
     </refgrp>
     <trd lng="be">&c_h;&c_ib;&c_n;&c_a;</trd>
     <trdgrp lng="de">
       <trd>Chinarinde</trd>
     </trdgrp>
     <trdgrp lng="en">
       <trd>cinchona</trd>,
       <trd>cinchona bark</trd>
     </trdgrp>
     <trd lng="fr">quinquina<klr tip="ind"> (&eacute;corce)</klr></trd>
     <trd lng="hu">k&iacute;nafak&eacute;reg</trd>
     <trd lng="nl">kinabast</trd>
     <trd lng="ru">&c_h;&c_i;&c_n;&c_a;</trd>
   </snc>
</drv>
</art>
