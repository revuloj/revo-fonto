<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kinkon</rad>/o
</kap>
<drv mrk="kinkon.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <uzo tip="fak">BOT</uzo>
    <dif>
      G. de rubiacoj, el Peruo, kiu liveras amaran kaj kontra&ubreve;febran &scirc;elon.
    </dif>
    <trdgrp lng="en">
      <trd>cinchona</trd>,
      <trd>cinchona tree</trd>
    </trdgrp>
    <trd lng="ru">&c_h;&c_i;&c_n;&c_n;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  </snc>
  <snc num="2" mrk="kinkon.0o.2">
    <uzo tip="fak">KEM</uzo>
    <dif>
      La &scirc;elo de la <tld/>o:
      <ekz>la <tld/>o estas ekspluatata pro siaj terapiaj proprecoj.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kinin.0o">kinino</ref>,
      <ref cel="kinkon1.0o">konkonino</ref>.
    </refgrp>
    <trdgrp lng="en">
      <trd>cinchona</trd>,
      <trd>cinchona bark</trd>
    </trdgrp>
    <trd lng="ru">&c_h;&c_i;&c_n;&c_a;</trd>
  </snc>
</drv>
</art>
