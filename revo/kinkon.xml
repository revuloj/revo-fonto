<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kinkon.xml,v 1.24 2023/12/03 09:58:49 revo Exp $">
<kap>
  <rad>kinkon</rad>/o
</kap>

<drv mrk="kinkon.0o">
  <kap><tld/>o</kap>
  <snc mrk="kinkon.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbo el la <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">genro</ref>
      de <ref tip="malprt" cel="rubi.0oj">rubiacoj</ref>
      el Peruo (<trd lng="la">Cinchona</trd>), havanta amaran
      &scirc;elon kontra&ubreve;febran:
      <ekz>
        &hcirc;inino estas alkaloido eltirebla el la planto <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19022">Kinino</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/6/66/Cinchona.pubescens01.jpg/600px-Cinchona.pubescens01.jpg" prm="PD">floraro de <tld/>o (Cinchona pubescens)
        <fnt>
          <aut>JoJan</aut>
          <lok><url ref="&WCW;/File:Cinchona.pubescens01.jpg">&WCW;/File:Cinchona.pubescens01.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="be">&c_h;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_je; &c_d;&c_r;&c_e;&c_v;&c_a;</trd>
    <trd lng="bg">&c_h;&c_i;&c_n;&c_i;&c_n;&c_o;&c_v;&c_o; &c_d;&c_malmol;&c_r;&c_v;&c_o;</trd>
    <trdgrp lng="de">
      <trd>Chinabaum</trd>,
      <trd>Chinarindenbaum</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>cinchona</trd>,
      <trd>cinchona tree</trd>
    </trdgrp>
    <trd lng="fr">quinquina<klr tip="ind"> (bot.)</klr></trd>
    <trd lng="hu">k&iacute;nafa</trd>
    <trd lng="nl">kinaboom</trd>
    <trd lng="pl">chinowiec</trd>     
    <trd lng="ro">chinchin&abreve;</trd>
    <trdgrp lng="ru">
      <trd>&c_h;&c_i;&c_n;&c_n;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>,
      <trd>&c_c;&c_i;&c_n;&c_h;&c_o;&c_n;&c_a;</trd>
    </trdgrp>
    <trd lng="zh">&#x91D1;&#x96DE;&#x7D0D;&#x6A39; <pr>j&#x12B;nj&#x12B;n&#xE0;sh&#xF9;</pr></trd>
</snc>
  <snc mrk="kinkon.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      La &scirc;elo de <tld/>o:
      <ekz>
        la <tld/>o estas uzata pro siaj ecoj terapiaj;
      </ekz>
      <ekz>
        principo <klr>[&#x2026;]</klr> proklamata de Hipokrato <klr>[&#x2026;]</klr> asertis, ke oni
        povas kuraci ian malsanon pere de la substanco, kiu estigas malsanon similan
        al tiu kuracenda <klr>[&#x2026;]</klr>, <nom>Hahnemann</nom> retrovis &gcirc;in, kiam li
        eksperimentis <klr>[&ccirc;e]</klr> si mem <klr>[per]</klr> <tld/>o kaj observis en
        sia organismo simptomojn similajn al tiuj de la mar&ccirc;a febro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=33316">Homeopatio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
      <ref cel="hxinin.0o">&hcirc;inino</ref>,
      <ref cel="kinkon1.0o">kinkonino</ref>.
    </refgrp>
    <trd lng="be">&c_h;&c_ib;&c_n;&c_a;</trd>
    <trd lng="bg">&c_h;&c_i;&c_n;&c_i;&c_n;&c_o;&c_v;&c_a; &c_k;&c_o;&c_r;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Chinarinde</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>cinchona</trd>,
      <trd>cinchona bark</trd>
    </trdgrp>
    <trd lng="fr">quinquina<klr tip="ind"> (&eacute;corce)</klr></trd>
    <trd lng="hu">k&iacute;nafak&eacute;reg</trd>
    <trd lng="nl">kinabast</trd>
    <trd lng="pl">china</trd> 
    <trd lng="ru">&c_h;&c_i;&c_n;&c_a;</trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x30AD;&#x30CA;&#x30CE;&#x30AD;</trd>,
    <trd>&#x30AD;&#x30CA;&#x76AE;  <pr>&#x30AD;&#x30CA;&#x3072;</pr></trd>
  </trdgrp>
  <trd lng="uk">&#x445;&#x456;&#x43D;&#x43D;&#x435; &#x434;&#x435;&#x440;&#x435;&#x432;&#x43E;</trd>
</drv>
</art>
