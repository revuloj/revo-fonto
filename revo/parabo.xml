<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc><rad>parabol</rad>/o
</kap>
<drv mrk="parabo.0o">
  <kap><tld/>o</kap>
  <snc mrk="parabo.0o.linio">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 33</lok></fnt>
    <dif>
      <ref tip="super" cel="konik.0o">Koniko</ref> kun discentreco
      egala al unu:
      <ekz>
        <tld/>o estas intersekco de konuso kun ebeno paralela al
        naskanto de &gcirc;i;
      </ekz>
      <ekz>
        <tld/>o havas unu fokuson;
      </ekz>
      <ekz>
        la kartezia ekvacio de <tld/>o estas de la tipo
        <frm><k>y</k> = <k>a</k><k>x</k><sup>2</sup></frm>;
      </ekz>
      <ekz>
        la trajektorio de pafa&jcirc;o estas <tld/>o.
      </ekz>
    </dif>
    <bld tip="svg" lok="../bld/k_parabolo.svg" alt="300" lrg="300">
      <tld/>o
    </bld>
    <trd lng="ca">par&agrave;bola<klr> (geom.)</klr></trd>
    <trd lng="en">parabola</trd>
    <trd lng="es">par&aacute;bola<klr> (geom.)</klr></trd>
    <trd lng="hu">parabola</trd>    
    <trd lng="nl">parabool</trd>
    <trd lng="pl">parabola</trd>
    <trd lng="pt">par&aacute;bola</trd>
    <trd lng="ru">&c_p;&c_a;&c_r;&c_a;&c_b;&c_o;&c_l;&c_a;</trd>
  </snc>
  <snc>
    <uzo tip="fak">BELE</uzo>
    <dif>
      Alegorio, sub kiu kasi&gcirc;as moralinstruo.
    </dif>
    <trd lng="nl">parabel</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_i;&c_t;&c_ch;&c_a;</trd>,
      <trd>&c_p;&c_a;&c_r;&c_a;&c_b;&c_o;&c_l;&c_a;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_b;&c_a;&c_l;&c_a;</trd>
  <trd lng="de">Parabel</trd>
  <trd lng="fr">parabole</trd>
  <trdgrp lng="hu">
    <trd>p&eacute;ld&aacute;zat</trd>,
    <trd>p&eacute;ldabesz&eacute;d</trd>,
    <trd>parabola</trd>
  </trdgrp>
</drv>
</art>
