<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dogm.xml,v 1.28 2023/11/04 18:50:51 revo Exp $">
<kap>
  <ofc>*</ofc> <ofc>2</ofc> <ofc>7</ofc>
  <rad>dogm</rad>/o
</kap>
<drv mrk="dogm.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="dogm.0o.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Principo de religia doktrino, esprimanta esencan parton de
      la religia kredo, kutime rigardata kiel revelaciita,
      nepridubebla (en la kadro de la koncerna kredo) kaj
      ne&scirc;an&gcirc;ebla
      (&scirc;an&gcirc;o en <tld/>o estigas novan
      <ref cel="konfes1.0o">konfesion</ref>):
      <ekz>
        la <tld/>o pri la Triunuo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la senmorteco de la animoj estas <tld/>o de la Platona filozofio
        <klr>(&ccirc;i tie pli konvenus <ref cel="aksiom.0o">aksiomo</ref>)</klr>.
      </ekz>
    </dif>
    <trd lng="be">&c_d;&c_a;&c_g;&c_m;&c_a;&c_t;</trd>
    <trd lng="bg">&c_d;&c_o;&c_g;&c_m;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Dogma</trd>,
      <trd>Glaubenssatz</trd>,
      <trd>Lehrsatz</trd>
    </trdgrp>
    <trd lng="es">dogma</trd>
    <trd lng="grc">&delta;&omikron_acute;&gamma;&my;&alfa;</trd>
    <trd lng="nl">dogma</trd>
    <trd lng="pl">dogmat</trd>
    <trd lng="ru">&c_d;&c_o;&c_g;&c_m;&c_a;&c_t;</trd>
    <trdgrp lng="zh">
    <trd>&#x6559;&#x4E49; <pr>ji&#xE0;oy&#xEC;</pr></trd>,
    <trd>&#x51C6;&#x5219; <pr>zh&#x1D4;nz&#xE9;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="dogm.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Principo, kiun oni ne rajtas kontra&ubreve;diskuti.
    </dif>
    <trd lng="be">&c_d;&c_o;&c_g;&c_m;&c_a;</trd>
    <trd lng="bg">&c_d;&c_o;&c_g;&c_m;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Dogma</trd>
    </trdgrp>
    <trd lng="ru">&c_d;&c_o;&c_g;&c_m;&c_a;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>dogma</trd>,
    <trd>&ccaron;l&aacute;nek v&iacute;ry</trd>
  </trdgrp>
  <trd lng="en">dogma</trd>
  <trd lng="fa">&a_d;&f_g;&a_m;</trd>
  <trd lng="fr">dogme</trd>
  <trd lng="hu">dogma</trd>
  <trdgrp lng="ja">
    <trd>&#x6559;&#x7FA9;  <pr>&#x304D;&#x3087;&#x3046;&#x304E;</pr></trd>,
    <trd>&#x6559;&#x7406;  <pr>&#x304D;&#x3087;&#x3046;&#x308A;</pr></trd>,
    <trd>&#x6559;&#x6761;  <pr>&#x304D;&#x3087;&#x3046;&#x3058;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x4FE1;&#x6761;  <pr>&#x3057;&#x3093;&#x3058;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x72EC;&#x65AD;  <pr>&#x3069;&#x304F;&#x3060;&#x3093;</pr></trd>,
    <trd>&#x72EC;&#x65AD;&#x7684;&#x4E3B;&#x5F35;  <pr>&#x3069;&#x304F;&#x3060;&#x3093;&#x3066;&#x304D;&#x3057;&#x3085;&#x3061;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">dogma</trd>
  <trd lng="pt">dogma</trd>
  <trd lng="sk">dogma</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&#x43E;&c_g;&#x43C;&c_a;&c_t;</trd>,
    <trd>&c_d;&#x43E;&c_g;&#x43C;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="dogm.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Apartenanta al <tld/>o.
    </dif>
    <trdgrp lng="fa">
      <trd>&a_j;&a_z;&a_m;&f_y;</trd>,
      <trd>&a_d;&f_g;&a_m;&a_A;&a_t;&f_y;&f_k;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Ne permesanta kontra&ubreve;diskuton.
    </dif>
    <trdgrp lng="fa">
      <trd>&a_j;&a_z;&a_m;&zwnj;&a_A;&a_n;&a_d;&f_y;&a_sh;</trd>,
      <trd>&a_d;&f_g;&a_m;<klr tip="ind"> (&a_S;&a_f;&a_t;)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_g;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="bg">&c_d;&c_o;&c_g;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_n;</trd>
  <trd lng="cs">dogmatick&yacute;</trd>
  <trdgrp lng="de">
    <trd>dogmatisch</trd>
  </trdgrp>
  <trd lng="en">dogmatic</trd>
  <trd lng="fr">dogmatique<klr> (adj.)</klr></trd>
  <trd lng="hu">dogmatikus</trd>
  <trdgrp lng="ja">
    <trd>&#x6559;&#x7FA9;&#x4E0A;&#x306E;  <pr>&#x304D;&#x3087;&#x3046;&#x304E;&#x3058;&#x3087;&#x3046;&#x306E;</pr></trd>,
    <trd>&#x72EC;&#x65AD;&#x7684;&#x306A;  <pr>&#x3069;&#x304F;&#x3060;&#x3093;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x6709;&#x7121;&#x3092;&#x8A00;&#x308F;&#x305B;&#x306A;&#x3044;  <pr>&#x3046;&#x3080;&#x3092;&#x3044;&#x308F;&#x305B;&#x306A;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">dogmatisch</trd>
  <trd lng="pl">dogmatyczny</trd>
  <trd lng="ru">&c_d;&c_o;&c_g;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">dogmatick&yacute;</trd>
  <trd lng="uk">&c_d;&#x43E;&c_g;&#x43C;&c_a;&c_t;&c_i;&c_ch;&#x43D;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x6B66;&#x65AD; <pr>w&#x1D4;du&#xE0;n</pr></trd>,
    <trd>&#x56FA;&#x57F7;&#x5DF2;&#x898B; <pr>g&#xF9;zh&#xED;y&#x1D0;ji&#xE0;n</pr></trd>,
    <trd>&#x6559;&#x6761; <pr>ji&#xE0;oti&#xE1;o</pr></trd>,
    <trd>&#x4E0D;&#x5BBD;&#x5BB9; <pr>b&#xF9;ku&#x101;nr&#xF3;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="dogm.0emo">
  <kap><tld/>emo</kap>
  <snc>
    <dif>
      Emo aserti a&ubreve;toritate sen sufi&ccirc;a kritiko.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_g;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_d;&c_o;&c_g;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_n;&c_o;&c_s;&c_t;</trd>
  <trd lng="cs">dogmati&ccaron;nost</trd>
  <trdgrp lng="de">
    <trd>zum Dogmatismus neigend</trd>,
    <trd>dogmatistisch</trd>
  </trdgrp>
  <trd lng="en">dogmatism</trd>
  <trdgrp lng="fa">
    <trd>&a_j;&a_z;&a_m;&zwnj;&a_A;&a_n;&a_d;&f_y;&a_sh;&f_y;</trd>,
    <trd>&a_d;&f_g;&a_m;&a_A;&a_t;&f_y;&a_s;&a_m;</trd>
  </trdgrp>
  <trd lng="fr">dogmatisme<klr tip="ind"> (attitude
  p&eacute;remptoire)</klr></trd>
  <trd lng="hu">dogmatizmus</trd>
  <trd lng="nl">dogmatisme</trd>
  <trd lng="sk">dogmatickos&tcaron;</trd>
  <trd lng="uk">&c_d;&#x43E;&c_g;&#x43C;&c_a;&c_t;&c_i;&c_ch;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>
  <trd lng="zh">&#x6559;&#x6761;&#x4E3B;&#x4E49; <pr>ji&#xE0;oti&#xE1;ozh&#x1D4;y&#xEC;</pr></trd>
</drv>

<drv mrk="dogm.0ismo">
  <kap><tld/>ismo</kap>
  <uzo tip="fak">FIL</uzo>
  <snc>
    <dif>
      Doktrino asertanta la eblecon de la kono pri la absoluto.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_g;&c_m;&c_a;&c_t;&c_y;&c_z;&c_m;</trd>
  <trd lng="bg">&c_d;&c_o;&c_g;&c_m;&c_a;&c_t;&c_i;&c_z;&c_malmol;&c_m;</trd>
  <trd lng="cs">dogmatizmus</trd>
  <trdgrp lng="de">
    <trd>Dogmatismus</trd>
  </trdgrp>
  <trd lng="en">dogmatics</trd>
  <trd lng="fr">dogmatique<klr> (subst.)</klr></trd>
  <trd lng="hu">dogmatizmus</trd>
  <trdgrp lng="ja">
    <trd>&#x72EC;&#x65AD;&#x8AD6;  <pr>&#x3069;&#x304F;&#x3060;&#x3093;&#x308D;&#x3093;</pr></trd>,
    <trd/>,
    <trd>&#x6559;&#x6761;&#x4E3B;&#x7FA9;  <pr>&#x304D;&#x3087;&#x3046;&#x3058;&#x3087;&#x3046;&#x3057;&#x3085;&#x304E;</pr></trd>
  </trdgrp>
  <trd lng="nl">dogmatisme</trd>
  <trd lng="sk">dogmatizmus</trd>
  <trd lng="uk">&c_d;&#x43E;&c_g;&#x43C;&c_a;&c_t;&c_i;&c_z;&#x43C;</trd>
  <trd lng="zh">&#x6559;&#x6761;&#x4E3B;&#x4E49; <pr>ji&#xE0;oti&#xE1;ozh&#x1D4;y&#xEC;</pr></trd>
</drv>

<drv mrk="dogm.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="dogm.0aro.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      <ref cel="doktri.0o">doktrino</ref>
      <ekz>
        ili <klr>...</klr> havis nek pastraron nek <tld/>aron, kaj
        &gcirc;enerale kunvenis en silento
        <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro 44a, p.
        101a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_a;&c_k;&c_t;&c_r;&c_y;&c_n;&c_a;</trd>,
      <trd>&c_v;&c_je;&c_r;&c_a;&c_v;&c_u;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trd lng="bg">&c_d;&c_o;&c_k;&c_t;&c_r;&c_i;&c_n;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Dogmatik</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>dogma</trd>,
      <trd>doctrine</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_A_madda;&a_m;&a_w;&a_z;&a_h;</trd>,
      <trd>&a_d;&f_k;&a_t;&a_r;&f_y;&a_n;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd><klr tip="ind">(le) </klr>dogme</trd>
      <trd>doctrine</trd>
    </trdgrp>
    <trd lng="hu">doktr&iacute;na</trd>
    <trd lng="ru">&c_v;&c_je;&c_r;&c_o;&c_u;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
    <trd lng="zh">&#x6559;&#x4E49;&#x5B66; <pr>ji&#xE0;oy&#xEC;xu&#xE9;</pr></trd>
</snc>
  <trdgrp lng="cs">
    <trd>doktr&iacute;na</trd>,
    <trd>soubor dogmat</trd>,
    <trd>v&ecaron;rouka</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6559;&#x7FA9;  <pr>&#x304D;&#x3087;&#x3046;&#x304E;</pr></trd>,
    <trd>&#x6559;&#x7406;  <pr>&#x304D;&#x3087;&#x3046;&#x308A;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>doctrine</trd>,
    <trd>leer<klr tip="amb"> (leerstellingen)</klr></trd>
  </trdgrp>
  <trd lng="sk">doktr&iacute;na</trd>
</drv>
</art>
