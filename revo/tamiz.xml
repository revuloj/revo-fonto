  <art mrk="$Id: tamiz.xml,v 1.9 2024/01/06 21:24:11 revo Exp $">
    <kap>
      <rad>Tamiz</rad>/o
    </kap>

<drv mrk="tamiz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="tamiz.0o.rivero">
    <dif>
      <ref tip="lst" lst="voko:riveroj" cel="river.0o.GEOG">Rivero</ref>

      trafluanta <ref cel="london.0o.urbo">Londono</ref>n:
      <ekz>
        post jarmiloj <klr>[&#8230;]</klr> <tld/>o, Danubo, Rejno ankora&ubreve;
        ruli&gcirc;as
        <fnt>
          <bib>Fab3</bib>
          <lok>Post jarmiloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        fronte al radikalaj islamismaj junuloj kun e&ubreve;ropaj pasportoj
        voja&gcirc;antaj en la
        &leftquot;sanktan militon&rightquot; kaj sangomakulite revenantaj al Rejno, Sejno
        kaj <tld/>o,
        li evidente fari&gcirc;is pli prudenta
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2015/011435.php">Konverti&gcirc;o?</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_T;&c_e;&c_m;&c_z;&c_a;</trd>
    <trd lng="bg">&c_T;&c_je;&c_m;&c_z;&c_a;</trd>
    <trd lng="de">Themse</trd>
    <trd lng="en">Thames</trd>
    <trd lng="fr">Tamise</trd>
  </snc>
  <trd lng="ja">&#x30C6;&#x30E0;&#x30BA;&#x5DDD; 
  <pr>&#x30C6;&#x30E0;&#x30BA;&#x304B;&#x308F;</pr></trd>
  <trd lng="pl">Tamiza</trd>
  <trd lng="uk">&c_T;&c_je;&#x43C;&c_z;&c_a;</trd>
</drv>
