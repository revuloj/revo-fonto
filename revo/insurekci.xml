<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>insurekci</rad>/i
</kap>
<drv mrk="insurekci.0i">
  <kap><tld/>i</kap>
  <snc mrk="insurekci.0i.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Grupe <ref tip="super" cel="ribel.0i.POL">ribeli</ref>,
      malobeante al &scirc;tata a&ubreve; alia oficiala a&ubreve;toritato.
    </dif>
  </snc>
  <trd lng="fr">s'<ind>insurger</ind></trd>
  <trdgrp lng="pl">
    <trd>powsta&cacute;<klr> przeciwko
komu&sacute;/czemu&sacute;</klr></trd>
    <trd>wznieca&cacute; powstanie</trd>
  </trdgrp>
  <trd lng="pt">insurgir(-se)</trd>
  <trd lng="ru">&c_v;&c_o;&c_s;&c_s;&c_t;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="insurekci.0o">
  <kap><tld/>o</kap> <fnt><bib>PIV1</bib></fnt>
  <snc mrk="insurekci.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Komuna <ref tip="super" cel="ribel.0o.POL">ribelo</ref> kontra&ubreve;
      politika a&ubreve;toritato:
      <ekz>
        la <tld/>o de la zapatistoj
        <fnt>
        <aut>E. Krebbers</aut>,
        <vrk><url
 
ref="http://www.gebladerte.nl/30035v01.htm">Pintkonferenco...</url></vrk>,
        <lok>2002-08-24</lok>
        </fnt>;
      </ekz>
      <ekz>
        la 19an de aprilo 1943 okazis <tld/>o en la Varsovia Getto
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;Varsovio&rightquot;,
        2006-04-27</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">insurrection</trd>
  <trdgrp lng="pl">
    <trd>insurekcja</trd>
    <trd>powstanie</trd>
  </trdgrp>
  <trd lng="pt">insurrei&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_v;&c_o;&c_s;&c_s;&c_t;&c_a;&c_n;&c_i;&c_je;</trd>
</drv>
</art>

