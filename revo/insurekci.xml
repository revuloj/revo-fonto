<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: insurekci.xml,v 1.16 2023/10/17 16:46:14 revo Exp $">
<kap>
  <rad>insurekci</rad>/i,
  <var><kap><rad var="t">insurekt</rad>/i <fnt><bib>NeoGlo</bib></fnt></kap></var>
</kap>
<drv mrk="insurekci.0i">
  <kap><tld/>i, <var><kap><tld var="t"/>i</kap></var></kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="insurekci.0i.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Kun<ref tip="super" cel="ribel.0i.POL">ribeli</ref>,
      fari popolribelon:
      <ekz>
        kontra&ubreve;antoj de <nom>Mobutu</nom> <tld/>is kaj en majo de 1997 la ribelo eniris en Kin&scirc;aso
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=244810">Mobutu Sese Seko</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld var="t"/>a anarkiismo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=141">Anarkiismo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
<!-- ne traduku evitindajn...
  <trd lng="cs">povstat</trd>
  <trd lng="de">rebellieren</trd>
  <trdgrp lng="nl">
    <trd>opstaan<klr tip="ind">(in opstand komen, ontr.</klr></trd>,
    <trd>in <ind>opstand</ind> komen<klr tip="ind"> (ontr.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>powsta&cacute;<klr> przeciwko
    komu&sacute;/czemu&sacute;</klr></trd>,
    <trd>wznieca&cacute; powstanie</trd>
  </trdgrp>
  <trd lng="pt">insurgir(-se)</trd>
  <trd lng="ru">&c_v;&c_o;&c_s;&c_s;&c_t;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">povsta&tcaron;</trd>
-->
</drv>

<drv mrk="insurekci.0o">
  <kap><tld/>o <fnt><bib>PIV1</bib></fnt>,
    <var><kap><tld var="t"/>o</kap></var>
  </kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="insurekci.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="ribel.popol0o.POL">Popolribelo</ref>:
      <ekz>
        la <tld/>o de la zapatistoj
        <fnt>
        <aut>E. Krebbers</aut>,
        <vrk><url ref="http://www.gebladerte.nl/30035v01.htm">
        Pintkonferenco...</url></vrk>,
        <lok>2002-08-24</lok>
        </fnt>;
      </ekz>
      <ekz>
        la 19an de aprilo 1943 okazis <tld/>o en la Varsovia <esc>Getto</esc>
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;Varsovio&rightquot;,
        2006-04-27</lok></fnt>;
      </ekz>
      <ekz>
        multaj <tld var="t"/>oj estis tie subpremitaj
        <fnt>
          <aut>J.-L. Racine, trad. V. Lutermano</aut>,
          <vrk><url ref="&Mondediplo;article1193.html">
          Va&scirc;ingtono kaj la talibanoj...</url></vrk>,
          <lok>le Monde diplomatique, 2007-02</lok>
        </fnt>;
      </ekz>
      <ekz>
        belorusoj malmulton scias pri la Varsovia <tld lit="I" var="t"/>o
        <fnt>
          <aut>Radio Polonia</aut>,
          <vrk><url ref="http://www.polskieradio.pl/eo/dokument.aspx?iid=57888">
          Ekspozicio dedi&ccirc;ita al la Varsovia Insurekcio</url></vrk>,
          <lok>2007-08-31</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
<!-- ne traduku evitindajn...
  <trdgrp lng="cs">
    <trd>bou&rcaron;e</trd>,
    <trd>insurekce</trd>,
    <trd>lidov&eacute; povst&aacute;n&iacute;</trd>,
    <trd>vzpoura</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Auftstand</trd>,
    <trd>Rebellion</trd>
  </trdgrp>
  <trd lng="nl">opstand<klr tip="ind"> (ontr.)</klr></trd>
  <trdgrp lng="pl">
    <trd>insurekcja</trd>,
    <trd>powstanie</trd>
  </trdgrp>
  <trd lng="pt">insurrei&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_v;&c_o;&c_s;&c_s;&c_t;&c_a;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>vzbura</trd>,
    <trd>&lcaron;udov&eacute; povstanie</trd>
  </trdgrp>
-->
  <trdgrp lng="ja">
    <trd>&#x6C11;&#x8846;&#x8702;&#x8D77;  <pr>&#x307F;&#x3093;&#x3057;&#x3085;&#x3046;&#x307B;&#x3046;&#x304D;</pr></trd>,
    <trd>&#x66B4;&#x52D5;  <pr>&#x307C;&#x3046;&#x3069;&#x3046;</pr></trd>,
    <trd>&#x53CD;&#x4E71;  <pr>&#x306F;&#x3093;&#x3089;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="uk">&#x456;&#x43D;&#x441;&#x443;&#x440;&#x435;&#x43A;&#x446;&#x456;&#x44F;</trd>
</drv>

<drv mrk="insurekci.0ano">
  <kap><tld/>ano, <var><kap><tld/>ulo</kap></var></kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="insurekci.0ano.POL">
    <dif>
      <ref tip="super" cel="ribel.0ulo.POL">Ribelulo</ref>, ano de 
      popolribelo:
      <ekz>
        li ekstaris kun sia armeo, unuigita kun la <tld/>anoj de 
        kolonelo <nom>Korponay</nom> 
        <fnt>
          <aut>T. Papp</aut>,
          <vrk>Debrecena batalo</vrk>,
          <lok>[<url ref="http://users.atw.hu/debrecenbulteno/Download/Numero%20014.doc">
          Debrecena Bulteno</url>, 1983, numero 14a]</lok>
        </fnt>;
      </ekz>
      <ekz>
        nokte germanoj <klr>[&#x2026;]</klr>alterigis 3 el 14 aviadiloj kun helpo al la <tld/>anoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=36494">Ribelo de Varsovio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
<!-- ne traduku evitindajn...
  <trdgrp lng="de">
    <trd>Aufst&auml;ndiger</trd>,
    <trd>Rebell</trd>
  </trdgrp>
  <trd lng="nl">opstandeling<klr tip="ind"> (ontr.)</klr></trd>
-->
</drv>
</art>
