    <refgrp tip="vid">
      <ref cel="trancx.dedekinda0o"><tld lit="d"/>a tran&ccirc;o</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Dedekindsch</trd>
  <trd lng="en">Dedekind['s]</trd>
  <trd lng="fr">de <ind>Dedekind</ind></trd>
  <trd lng="ru">&c_D;&c_je;&c_d;&c_je;&c_k;&c_i;&c_n;&c_d;&c_a;</trd>
</drv>
</art>

