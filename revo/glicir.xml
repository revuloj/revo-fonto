<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: glicir.xml,v 1.28 2023/11/11 11:36:35 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>gliciriz</rad>/o
</kap>
<drv mrk="glicir.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="glicir.0o.genro">
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Glycyrrhiza</trd>), el kies radiko oni ekstraktas
      dol&ccirc;an sukon, uzatan kontra&ubreve; tuso kaj por dol&ccirc;a&jcirc;oj:
      <ekz>
        oni uzas dekon da diversaj spicherboj por la trinka&jcirc;o, oni
        mencias centa&ubreve;r<klr>[e]</klr>on, <tld/>on, santallignon, 
        gencianoradikon, la&ubreve;rofolion kaj juniperberon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=445202">Beerenburg</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        Blanka trufo el Italio havas <tld/>an guston
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=20951">Trufo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/a/ae/Glycyrrhiza_lepidota_(4007533989).jpg/640px-Glycyrrhiza_lepidota_(4007533989).jpg" prm="CC BY-SA 2.0">floraroj de <tld/>o (Glycyrrhiza lepidota)
        <fnt>
          <aut>Matt Lavin el Bozeman, Montana, Usono</aut>
          <lok><url ref="&WCW;/File:Glycyrrhiza_lepidota_(4007533989).jpg">&WCW;/File:Glycyrrhiza_lepidota_(4007533989).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_l;&c_a;&c_d;&c_k;&c_a;&c_k;&c_o;&c_r;&c_a;&c_n;&c_mol;</trd>,
    <trd>&c_l;&c_a;&c_k;&c_r;&c_y;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>regal&iacute;ssia</trd>,
    <trd>regal&egrave;ssia</trd>
  </trdgrp>
  <trd lng="de">S&uuml;&szlig;holz</trd>
  <trdgrp lng="en">
    <trd>liquorice</trd>,
    <trd>licorice</trd>
  </trdgrp>
  <trd lng="es">regaliz</trd>
  <trd lng="fa">&a_sh;&f_y;&a_r;&f_y;&a_n;&zwnj;&a_b;&f_y;&a_A;&a_n;</trd>
  <trd lng="fr">r&eacute;glisse<klr tip="ind"> (plante)</klr></trd>
  <trd lng="ja">&#x30AB;&#x30F3;&#x30BE;&#x30A6;</trd>
  <trd lng="nl">zoethout</trd>
  <trd lng="pt">alca&ccedil;uz<klr tip="amb"> (bot&acirc;nica)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_l;&c_o;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_l;&c_a;&c_k;&c_r;&c_i;&c_ch;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="tr">meyank&ouml;k&uuml;</trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x43E;&#x43B;&#x43E;&#x434;&#x435;&#x446;&#x44C;</trd>,
    <trd>&#x441;&#x43E;&#x43B;&#x43E;&#x434;&#x43A;&#x430;</trd>,
    <trd>&#x441;&#x43E;&#x43B;&#x43E;&#x434;&#x43A;&#x438;&#x439; &#x43A;&#x43E;&#x440;&#x456;&#x43D;&#x44C;</trd>,
    <trd>&#x43B;&#x43E;&#x43A;&#x440;&#x438;&#x446;&#x44F;</trd>
  </trdgrp>
  <trd lng="zh">&#x82F7; <pr>g&#x101;n</pr></trd>
</drv>

<drv mrk="glicir.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc mrk="glicir.0ajxo.lakrico">
    <dif>
      <ref tip="dif" cel="lakric.0o">Lakrico</ref>:
      <ekz>
        lakrico a&ubreve; <tld/>a&jcirc;o estas dol&ccirc;a&jcirc;o farita el la ekstrakto de
        la vera <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="&Viki;/Lakrico">
            Lakrico
          </url></lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      Kvankam &leftquot;<tld/>a&jcirc;o&rightquot; povus nomi iun ajn produkton
      faritan el <tld/>o, ekz. <tld/>an tizanon, komune
      oni celas la nigran franda&jcirc;on konatan anka&ubreve; kiel
      &leftquot;lakrico&rightquot;.
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_k;&c_r;&c_y;&c_c;&c_a;</trd>,
    <trd>&c_s;&c_a;&c_l;&c_o;&c_d;&c_k;&c_a;&c_ja; &c_g;&c_r;&c_a;&c_z;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">pega dol&ccedil;a</trd>
  <trdgrp lng="de">
    <trd>Lakritz</trd>,
    <trd>Lakritze</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>liquorice</trd>,
    <trd>licorice</trd>
  </trdgrp>
  <trd lng="fr">r&eacute;glisse<klr tip="ind"> (confiserie)</klr></trd>
  <trd lng="nl">drop</trd>
  <trd lng="ru">&c_l;&c_a;&c_k;&c_r;&c_i;&c_c;&c_a;</trd>
  <trd lng="zh">&#x7518;&#x8349; <pr>g&#x101;nc&#x1CE;o</pr></trd>
</drv>
</art>
