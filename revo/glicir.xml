<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>gliciriz</rad>/o
    </kap>
    <uzo tip="fak">BOT</uzo>
    <drv mrk="glicir.0o">
      <kap><tld/>o</kap>
      <snc>
	<dif>
	  Veg. el fam. de fabacoj, el kies radiko oni ekstraktas
	  dol&ccirc;an sukon, uzatan kontra&ubreve; tuso a&ubreve; en medikamentoj (<trd
	  lng="la">glycyrrhiza</trd>).
	</dif>
      </snc>
      <trd lng="fr">r&eacute;glisse</trd>
      <trd lng="nl">zoethout</trd>
    </drv>
    <drv mrk="glicir.0ajxo">
      <kap><tld/>a&jcirc;o</kap>
      <snc>
	<dif>
	  Dol&ccirc;a&jcirc;o nigra, farita el infuza&jcirc;o de <tld/>o. La
	  gusto povas varii de tre sala &gcirc;is tre dol&ccirc;a, kaj de
	  sufi&ccirc;e mola &gcirc;is sufi&ccirc;e malmola.
	</dif>
      </snc>
      <bld lok="http://131.211.121.124/REVO/NovajBildoj/glicirizajxo.jpg">
	Kelkaj el &ccirc;i tiuj dol&ccirc;a&jcirc;oj.
      </bld>
      <trd lng="nl">drop</trd>
      <trdgrp lng="en">
	<trd>liquorice</trd>
	<trd>licorice</trd>
      </trdgrp>
    </drv>
    
  </art>
