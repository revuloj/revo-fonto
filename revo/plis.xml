<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: plis.xml,v 1.5 2022/05/01 17:51:19 revo Exp $">
    <kap>
      <rad>plis</rad>/i
    </kap>

<drv mrk="plis.0i">
  <kap><tld/>i</kap>
  <snc mrk="plis.0i.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="dif" cel="fald.0umi">Faldumi</ref>:
      <ekz>
        por la kolumeto, manikoj kaj pantalonaj refalda&jcirc;oj estas bezonata 150&#8239;-&#8239;200&#8239;m da
        <tld/>ita rubando
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/006955.html">Belgio: Fama karnavalo en
          Binche</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        blanka robo, <tld/>ita surbruste, zonita per blua rubando
        <fnt>
          <aut>Lev Tolstoj</aut>
          <vrk><url ref="&LOdE;Rn-tlst2.htm">Resurekto (fragmento)</url></vrk>
          <lok>La Ondo de Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref cel="krisp.0o.TEKS">krispo</ref> estas kolumo, simpla a&ubreve; duobla, el <tld/>ita a&ubreve;
        rondfaldetita amelita
        tolo a&ubreve; punto
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Krispo">Krispo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="krep.0o">krepo</ref>
    <trd lng="de">plissieren</trd>
    <trd lng="fr">plisser</trd>
  </snc>
  <trd lng="en">pleat</trd>
  <trd lng="pl">plisowa&cacute;</trd>
  <trdgrp lng="ro">
    <trd>plisa</trd>,
    <trd>kilt</trd>
  </trdgrp>
  <trd lng="uk">&c_p;&c_l;&c_ib;&c_s;&c_i;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

  </art>

