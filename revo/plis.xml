<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: plis.xml,v 1.8 2023/12/27 07:24:51 revo Exp $">
    <kap>
      <rad>plis</rad>/i
    </kap>

<drv mrk="plis.0i">
  <kap><tld/>i</kap>
  <snc mrk="plis.0i.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="dif" cel="fald.0umi">Faldumi</ref>:
      <ekz>
        por la kolumeto, manikoj kaj pantalonaj refalda&jcirc;oj estas bezonata
        150&#x202F;-&#x202F;200&#x202F;m da
        <tld/>ita rubando
        <fnt>
          <aut>Maxime Lechien</aut>
          <vrk><url ref="&Monato;/2000/006955.php">Fama karnavalo en Binche</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        blanka robo, <tld/>ita surbruste, zonita per blua rubando
        <fnt>
          <aut>Lev Tolstoj</aut>
          <vrk><url ref="&LOdE;Rn-tlst2.htm">Resurekto (fragmento)</url></vrk>
          <lok>La Ondo de Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref cel="krisp.0o.TEKS">krispo</ref> estas kolumo, simpla a&ubreve; duobla, el <tld/>ita a&ubreve;
        rondfaldetita amelita
        tolo a&ubreve; punto
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Krispo">Krispo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="krep.0o">krepo</ref>
    <trd lng="be">&c_p;&c_l;&c_ib;&c_s;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="de">plissieren</trd>
    <trd lng="fr">plisser</trd>
  </snc>
  <trd lng="en">pleat</trd>
  <trdgrp lng="ja">
    <trd>&#x30D7;&#x30EA;&#x30FC;&#x30C4;&#12434;&#12388;&#12369;&#x308B;</trd>,
    <trd>&#12402;&#12384;&#12434;&#12388;&#12369;&#x308B;</trd>
  </trdgrp>
  <trd lng="pl">plisowa&cacute;</trd>
  <trdgrp lng="ro">
    <trd>plisa</trd>,
    <trd>kilt</trd>
  </trdgrp>
  <trd lng="uk">&c_p;&c_l;&c_ib;&c_s;&c_i;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

  </art>

