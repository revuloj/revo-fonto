<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: altometr.xml,v 1.7 2024/12/17 08:37:08 revo Exp $">
<kap>
  <rad>altometr</rad>/o <fnt><bib>PIV1</bib></fnt>,
  <var><kap><rad var="i">altimetr</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>
<drv mrk="altometr.0o">
  <kap><tld/>o,
  <var><kap><tld var="i"/>o</kap></var></kap>
  <snc mrk="altometr.0o.MEZ">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="dif" cel="mezur.alto0ilo.AVI">Altomezurilo</ref>:
      <ekz>
        la <tld/>o montris da&ubreve;ran ascendon, kaj ili flugadis
        konstante la&ubreve; sama kurso
        <fnt>
          <aut>J. Francis</aut>
          <vrk>La Granda Kaldrono</vrk>
          <lok>1978</lok>
        </fnt>;
      </ekz>
      <ekz>
        li a&ubreve;dis klare la tiktakadon de la <tld var="i"/>o mezuranta la kilometrojn
        apartigantajn lin de la planedsurfaco
        <fnt>
          <aut>K. Fia&lstroke;kowski, trad. Geo</aut>
          <vrk><url ref="https://eduinf.waw.pl/esp/lern/librejo/0409/0020.php">
          Kosmodromo</url></vrk>
          <lok>2023</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_t;&c_y;&c_m;&c_e;&c_t;&c_a;&c_r;</trd>
  <trdgrp lng="de">
    <trd>H&ouml;henmesser</trd>,
    <trd>Altimeter</trd>
  </trdgrp>
  <trd lng="fr">altim&egrave;tre</trd>
  <trd lng="ja">&#x9AD8;&#x5EA6;&#x8A08;  <pr>&#12371;&#12358;&#12393;&#12369;&#12356;</pr></trd>
  <trd lng="pl">wysoko&sacute;ciomierz</trd>
</drv>
</art>

