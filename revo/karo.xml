<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>karo</rad>/o
    </kap>
    <drv mrk="karo.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <uzo tip="fak">SPO</uzo>
      <snc>
        <dif>
          Ludkarta
          <ref tip="super" cel="emblem.0o.karta">emblemo</ref>,
          signata per ru&gcirc;a <ref tip="vid" cel="romb.0o">rombo</ref>
          (&#x2662;):
          <ekz>
            <tld/>a aso.
          </ekz>
        </dif>
        <trd lng="en">diamond</trd>
        <trd lng="nl">ruiten <klr tip="ind">(in kaartspel)</klr></trd>
        <trd lng="en">&c_b;&c_u;&c_b;&c_n;&c_y; <klr>(&c_m;&c_a;&c_s;&c_t;&c_mol;)</klr></trd>
      </snc>
    </drv>
  </art>
