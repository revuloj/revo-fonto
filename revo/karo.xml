<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: karo.xml,v 1.26 2024/03/12 15:23:33 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>karo</rad>/o
</kap>
<drv mrk="karo.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      Ludkarta
      <ref tip="super" cel="emblem.0o.karta">emblemo</ref>,
      signata per ru&gcirc;a <ref tip="vid" cel="romb.0o">rombo</ref>
      (&#9826;):
      <ekz>
        <tld/>a aso.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ker.0o">kero</ref>,
      <ref cel="pik.0o.emblemo">piko</ref>,
      <ref cel="tref.0o">trefo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_v;&c_o;&c_n;&c_k;&c_ib; <klr>(&c_u; &c_k;&c_a;&c_r;&c_t;&c_a;&c_h;)</klr></trd>,
    <trd>&c_b;&c_u;&c_b;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="br">karo</trd>
  <trdgrp lng="cs">
    <trd>karo</trd>,
    <trd>kule <klr>(karty)</klr></trd>
  </trdgrp>
  <trd lng="de">Karo</trd>
  <trd lng="en">diamond</trd>
  <trd lng="es">diamantes <klr>(palo de la baraja de naipes
  francesa)</klr></trd>
  <trd lng="fr">carreau<klr tip="ind"> (jeu de cartes)</klr></trd>
  <trd lng="hu">k&aacute;r&oacute;</trd>
  <trd lng="ja">&#x30C0;&#x30A4;&#x30E4;  <pr>&#12384;&#12356;&#12420;</pr></trd>
  <trd lng="nl">ruiten<klr tip="ind"> (in kaartspel)</klr></trd>
  <trdgrp lng="pl">
    <trd>karo</trd>,
    <trd>poduszki<klr> (pot.)</klr></trd>,
    <trd>dzwonek<klr> (pot.)</klr></trd>
  </trdgrp>
  <trd lng="ro">caro</trd>
  <trd lng="ru">&c_b;&c_u;&c_b;&c_n;&c_y;
  <klr>(&c_m;&c_a;&c_s;&c_t;&c_mol;)</klr></trd>
  <trdgrp lng="sk">
    <trd>gu&lcaron;a <klr>(karty)</klr></trd>,
    <trd>k&aacute;ro</trd>
  </trdgrp>
  <trd lng="sv">ruter</trd>
  <trd lng="uk">&c_b;&c_u;&c_b;&#x43D;&c_a;</trd>
  <trd lng="zh">&#x65B9;&#22359; <pr>f&amacron;ngku&#xE0;i</pr></trd>
</drv>
</art>
