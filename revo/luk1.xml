<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: luk1.xml,v 1.13 2022/03/22 18:26:51 revo Exp $">
<kap>
  <rad>Luk</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="luk1.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        <ctl>la evangelio la&ubreve; Sankta <tld/>o</ctl>
        <fnt><bib>NT</bib>, <lok>&Luk;</lok></fnt>
      </ekz>
    </dif>
  </snc>          
  <ref tip="hom" cel="luk.0o">luko (aperturo)</ref>
  <trdgrp lng="be">
    <trd>&c_L;&c_u;&c_k;&c_a;</trd>,
    <trd>&c_L;&c_u;&c_k;&c_a;&c_sh;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>Lug</trd>,
    <trd>Lukaz</trd>
  </trdgrp>
  <trd lng="de">Lukas</trd>
  <trd lng="es">Lucas</trd>
  <trd lng="fr">Luc</trd>
  <trd lng="hu">Luk&aacute;cs</trd>
  <trdgrp lng="nl">
    <trd>Lucas</trd>,
    <trd>Luk</trd>
  </trdgrp>
  <trd lng="pl">&Lstroke;ukasz</trd>
  <trd lng="pt">Lucas</trd>
  <trd lng="ro">Luca</trd>
  <trd lng="ru">&c_L;&c_u;&c_k;&c_a;</trd>
  <trd lng="uk">&#x41B;&#x443;&#x43A;&#x430;</trd>
</drv>
</art>

