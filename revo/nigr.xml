<drv mrk="nigr.0o">
  <kap><tld/>o</kap>
  <snc mrk="nigr.0o.KOL">
    <dif>
      <tld lit="N"/>a <ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">koloro</ref>:
      <ekz>
        li havis <klr>[&#8230;]</klr> anta&ubreve;tukon, bluan kun <tld/>o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en &ccirc;i tiu &leftquot;vivo&rightquot; ne estis promontoro kaj valo,
        amo kaj malamo, blanko kaj <tld/>o <klr>(t.e. kontrasto, ekscito)</klr>,
        nur senfina, griza vakuo
        <fnt>
          <bib>VaK</bib>
          <lok>Sesa &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
	<tld/>o sur blanko <klr>(= skriba&jcirc;o)</klr> pruvas sen
	manko
	<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la <tld/>o de la nokto ankora&ubreve; ne regas kun absoluta forto
        <fnt>
          <bib>KKr</bib>
          <lok>II</lok>
        </fnt>;
      </ekz>
      <ekz>
        maldika viro en <tld/>o
        <klr>(<tld/>a vesto)</klr>
        <fnt>
          <bib>MkM</bib>
          <lok>22</lok>
        </fnt>;
      </ekz>
      <ekz>
        paradokse, post tiom da <tld/>o <klr>(<ref cel="melank.0o">melankolio</ref>)</klr>
        kaj amaro enanime fine restas plejparte helo
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#8470;10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="nigr.0o.ludanto">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Unu el la du ludantoj en ludoj kiel <ref cel="sxak.0o">&scirc;ako</ref>,
      <ref cel="go.0o">goo</ref>, <ref cel="dam.0oj0ludo">damoj</ref>,
      ktp.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_r;&c_n;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_ch;&c_o;&c_r;&c_n;&c_y; &c_k;&c_o;&c_l;&c_je;&c_r;</trd>
  </trdgrp>
  <trd lng="bg">&c_ch;&c_je;&c_r;&c_je;&c_n; &c_c;&c_v;&c_ja;&c_t;</trd>
  <trd lng="br">du<klr tip="ind"> (ak.)</klr></trd>
  <trdgrp lng="cs">
    <trd>&ccaron;ern&aacute; barva</trd>,
    <trd>&ccaron;er&ncaron;</trd>
  </trdgrp>
  <trd lng="de">Schw&auml;rze</trd>
  <trd lng="en">black</trd>
  <trd lng="es">negro<klr> (subst. color)</klr></trd>
  <trd lng="fr">noir<klr> (subst. couleur)</klr></trd>
  <trd lng="id">hitam</trd>
  <trd lng="ja">&#40658; <pr>&#12367;&#12429;</pr></trd>
  <trd lng="ku">re&scedil;</trd>
  <trd lng="nl">zwart<klr tip="ind"> (zn.)</klr></trd>
  <trd lng="pl">czer&nacute;</trd>
  <trdgrp lng="pt">
    <trd>negro</trd>,
    <trd>preto</trd>
  </trdgrp>
  <trd lng="ro">negru</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_r;&c_n;&c_o;&c_t;&c_a;</trd>,
    <trd>&c_ch;&c_jo;&c_r;&c_n;&c_y;&c_j; &c_c;&c_v;&c_je;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">&ccaron;er&ncaron;</trd>
  <trd lng="zh">&#40657;&#26263; <pr>h&emacron;i&agrave;n</pr></trd>
</drv>

<drv mrk="nigr.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="nigr.0ajxo.KOL">
    <dif>
      Io <tld/>a a&ubreve; <tld/>a parto de io:
      <ekz>
        estas tiom same prudente preni serioze presan <tld/>a&jcirc;on kaj paperon, kiel &ccirc;ian
        alian aferon
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_r;&c_n;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_ch;&c_o;&c_r;&c_n;&c_a;&c_je; <klr>(&c_n;&c_a;&c_z;.)</klr></trd>,
    <trd>&c_ch;&c_o;&c_r;&c_n;&c_a;&c_ja; <ind>&c_p;&c_l;&c_ja;&c_m;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="de">Schwarzes</trd>
  <trd lng="en">black</trd>
  <trd lng="es">negro</trd>
  <trdgrp lng="ja">
    <trd>&#40658;&#12356;&#37096;&#20998;  <pr>&#12367;&#12429;&#12356;&#12406;&#12406;&#12435;</pr></trd>,
    <trd>&#12424;&#12372;&#12428;</trd>
  </trdgrp>
  <trd lng="nl">iets <ind>zwart</ind>s</trd>
  <trdgrp lng="pl">
    <trd>czarna plama</trd>,
    <trd>czer&nacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>pat&abreve; neagr&abreve;</trd>,
    <trd>plama neagr&abreve;</trd>,
    <trd>negru</trd>
  </trdgrp>
</drv>

<drv mrk="nigr.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="nigr.0igi.KOL">
    <dif>
      Fari <tld/>a:
      <ekz>
	<tld/>igi grizajn harojn;
      </ekz>
      <ekz>
        la kurtenoj subite fajri&gcirc;is en flugado de flamoj, kiuj <tld/>igis la plafonon
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igi malplenajn kvadratojn
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#8470; 6 (104)</lok>
        </fnt>;
      </ekz>
      <ekz>
	<tld/>aj traboj transsaltis super niaj kapoj, kun
        <ind>fum<tld/>igita</ind> plafono super ili
        <fnt>
          <aut>A. Giridhar Rao</aut>
          <vrk>La triumfo de rezono</vrk>
          <bib>Monato</bib>
          <lok>2001</lok>
        </fnt>.
        <trd lng="nl">zwartgeblakerd</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_ch;&c_a;&c_r;&c_n;&c_ib;&c_c;&c_mol;</trd>
    <trd lng="de">schw&auml;rzen</trd>
    <trd lng="en">blacken</trd>
    <trd lng="es">ennegrecer</trd>
    <trdgrp lng="fr">
      <trd>noircir</trd>
    </trdgrp>
    <trd lng="id">meng<ind>hitam</ind>kan</trd>
    <trd lng="nl"><ind>zwart</ind> maken</trd>
    <trd lng="pl">czerni&cacute;</trd>
    <trd lng="ro">&icirc;nnegri</trd>
    <trd lng="ru">&c_ch;&c_je;&c_r;&c_n;&c_i;&c_t;&c_mol;</trd>
    <trd lng="tr">karalamak</trd>
  </snc>
  <snc mrk="nigr.0igi.FIG_malgajigi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malgajigi:
      <ekz>
	la malsano <tld/>igis lian spiriton, vivon;
      </ekz>
      <ekz>
        <klr>[mi]</klr> murdos &ccirc;ion, kio <tld/>igas mian tagon
        <fnt>
          <bib>Monato</bib>
          <aut>Geraldo Mattos</aut>
          <vrk><url ref="&Monato;/2008/009422.php"
              >Rezulto de preseraro</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_z;&c_m;&c_r;&c_o;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="de">verd&uuml;stern</trd>
    <trd lng="es">entristecer</trd>
    <trdgrp lng="fr">
      <trd>attrister</trd>,
      <trd>noircir<klr> (attrister)</klr></trd>
    </trdgrp>
