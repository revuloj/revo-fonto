<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>nigr</rad>/a
</kap>
<drv mrk="nigr.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      De koloro plej kontra&ubreve;a al blanko, samkolora kiel karbo kaj fulgo:
      <ekz>
        <tld/>a kiel inko, korva flugilo;
      </ekz>
      <ekz>
        <tld/>a frako;
      </ekz>
      <ekz>
        <tld/>e vestita;
      </ekz>
      <ekz>
        karbo<tld/>a<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="tr">
      <trd>kara</trd>,
      <trd>siyah</trd>
    </trdgrp>
    <trd lng="vo">bl&auml;gik</trd>
  </snc>
  <snc>
    <dif>
      Malluma:
      <ekz>
        <tld/>a nokto;
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Funebra, malgaja, pesimisma:
      <ekz>
        <tld/>a humoro;
      </ekz>
      <ekz>
        <tld/>a (profunda) mizero.
      </ekz>
      <ekz>
        <tld/>aj ideoj
        (<ref tip="sin" cel="melank.0o">melankolio</ref>);
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vid.nigre0ulo"><tld/>evidulo</ref>
    </refgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malicega, abomeninda:
      <ekz>
        <tld/>a animo, koro, krimo;
      </ekz>
      <ekz>
        vi, <tld/>aj semantoj de malpaco<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="tr">kara</trd>
  </snc>                     
  <trd lng="be">&c_ch;&c_o;&c_r;&c_n;&c_y;</trd>
  <trd lng="br">du<klr tip="ind"> (ag.)</klr></trd>
  <trd lng="cs">&ccaron;ern&yacute;</trd>
  <trd lng="de">schwarz</trd>
  <trd lng="el">&my;&alfa;&ypsilon_ton;&rho;&omikron;&sigma_fina;</trd>
  <trd lng="en">black</trd>
  <trd lng="es">negro</trd>
  <trd lng="fr">noir</trd> 
  <trd lng="nl">zwart</trd>
  <trd lng="pl">czarny</trd>
  <trdgrp lng="pt">
    <trd>negro</trd>,
    <trd>preto</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_jo;&c_r;&c_n;&c_y;&c_j;</trd>
  <trd lng="sv">svart</trd>
</drv>

<drv mrk="nigr.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="N"/>a koloro:
      <ekz>
        <tld/>o sur blanko (= skriba&jcirc;o) pruvas sen
        manko
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_r;&c_n;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_ch;&c_o;&c_r;&c_n;&c_y; &c_k;&c_o;&c_l;&c_je;&c_r;</trd>
  </trdgrp>           
  <trd lng="br">du<klr tip="ind"> (ak.)</klr></trd>
  <trd lng="de">Schw&auml;rze</trd>
  <trd lng="es">negro</trd>
  <trd lng="fr">noir<klr> (subst. couleur)</klr></trd>
  <trd lng="nl">zwart</trd>
  <trdgrp lng="pt">
    <trd>negro</trd>,
    <trd>preto</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_r;&c_n;&c_o;&c_t;&c_a;</trd>,
    <trd>&c_ch;&c_jo;&c_r;&c_n;&c_y;&c_j; &c_c;&c_v;&c_je;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="nigr.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      <tld lit="N"/>a parto de io.
    </dif>
  </snc>
  <trd lng="de">Schwarzes</trd>
</drv>

<drv mrk="nigr.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari <tld/>a:
      <ekz>
        <tld/>igi grizajn harojn.
      </ekz>
    </dif>
    <trd lng="be">&c_ch;&c_a;&c_r;&c_n;&c_ib;&c_c;&c_mol;</trd>
    <trd lng="de">schw&auml;rzen</trd>
    <trd lng="es">ennegrecer</trd>
    <trdgrp lng="fr">
      <trd>noircir</trd>
    </trdgrp>
    <trd lng="nl">zwart maken</trd>
    <trd lng="ru">&c_ch;&c_je;&c_r;&c_n;&c_i;&c_t;&c_mol;</trd>
    <trd lng="tr">karalamak</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malgajigi:
      <ekz>
        la malsano <tld/>igis lian spiriton, vivon.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_z;&c_m;&c_r;&c_o;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="de">verdunkeln</trd>
    <trdgrp lng="fr">
      <trd>attrister</trd>,
      <trd>noircir<klr> (attrister)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_m;&c_r;&c_a;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malbonfamigi:
      <ekz>
        kalumniante konstante, oni e&ccirc; an&gcirc;elojn
        <tld/>igas<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>igi ies intencojn.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_ch;&c_a;&c_r;&c_n;&c_ja;&c_c;&c_mol;</trd>
    <trd lng="de">anschw&auml;rzen</trd>
    <trdgrp lng="fr">
      <trd>calomnier</trd>,
      <trd>m&eacute;dire de</trd>,
      <trd>noircir<klr> (calomnier)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_ch;&c_je;&c_r;&c_n;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="br">dua&ntilde;</trd>
</drv>

<drv mrk="nigr.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <ref tip="dif" cel="negr.0o">negro</ref>
    <ekz>
      la bonhumora &ccirc;eesto de la <tld/>uloj vere mankis al mi poste
      <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro 10a, p. 34a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_e;&c_g;&c_a;&c_r;</trd>,
    <trd>&c_ch;&c_a;&c_r;&c_n;&c_a;&c_s;&c_k;&c_u;&c_r;&c_y;</trd>
  </trdgrp>              
  <trd lng="br">morian</trd>
  <trdgrp lng="de">
    <trd>Schwarzer</trd>,
    <trd>Neger</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&kaf;&vav;&shin;&yod;</trd>,
    <trd>&shin;&het;&vav;&resh;<klr tip="ind"> (&kaf;&vav;&shin;&yod;)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>n&eacute;ger</trd>,
    <trd>fekete</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zwarte</trd>,
    <trd>neger</trd>
  </trdgrp>
  <trd lng="pt">negro</trd>
  <trd lng="ru">&c_n;&c_je;&c_g;&c_r;</trd>
  <trd lng="sv">neger</trd>
</drv>

<drv mrk="nigr.N0aMaro">
  <kap><tld lit="N"/>a Maro</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Interna maro
      <ref tip="malprt" cel="atlant.0o">Atlantika</ref>
      inter E&ubreve;ropo kaj Azio, &ccirc;e la bordoj de
      <ref cel="rus.R0ujo">Rusujo</ref>, Ukrainujo, Rumanujo,
      <ref cel="bulgar.B0ujo">Bulgarujo</ref>,
      <ref cel="turk.T0ujo">Turkujo</ref> kaj
      <ref cel="kartvel.K0ujo">Kartvelujo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_Ch;&c_o;&c_r;&c_n;&c_a;&c_je; &c_m;&c_o;&c_r;&c_a;</trd>    
  <trd lng="br">Mor Du</trd>
  <trd lng="bg">&c_Ch;&c_je;&c_r;&c_n;&c_o; &c_m;&c_o;&c_r;&c_je;</trd>
  <trd lng="de">Schwarzes Meer</trd>
  <trd lng="fr">Mer Noire</trd>
  <trd lng="pt">Mar Negro</trd>
  <trd lng="ru">&c_Ch;&c_jo;&c_r;&c_n;&c_o;&c_je; &c_m;&c_o;&c_r;&c_je;</trd>
  <trd lng="tr">Karadeniz</trd>
</drv>
</art>
