<drv mrk="nigr.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="nigr.0a.KOL">
    <dif>
      Sam<ref tip="super" cel="kolor.0o.propre">kolora</ref> kiel karbo kaj fulgo; sorbanta ricevatan lumon kaj
      ne reflektanta &gcirc;in en la videbla spektro:
      <ekz>
        haroj <klr>[&#x2026;]</klr> <tld/>aj kiel korvo
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 5:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        talpo en <klr>[&#x2026;]</klr> <tld/>a velura pelto
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        grandaj <tld/>aj okuloj
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        aperis granda <tld/>a <klr>(tre malhela)</klr> nubo
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj karbigitaj traboj, kiuj apogis sin al la kamentubo
        <fnt>
          <bib>Fab2</bib>
          <lok>Najbaraj familioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj malaperis internen en la nigran teron, tiel ke oni ne povis plu vidi ian
        signon
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        li &scirc;miris al si la viza&gcirc;on per bruna kaj <tld/>a farbo
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tisto de porkoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        sed post kelkaj tagoj li plene hejmi&gcirc;is inter la
        <ind>fulgo<tld/>a</ind>j viroj de la Torentaj uzinoj
         <fnt><aut>S. Engholm</aut>, <vrk>Al
         Torento</vrk>, <lok>1930</lok></fnt>;
        <trd lng="nl">gitzwart</trd>
      </ekz>
      <ekz>
        <tld/>an kornikon sapo ne blankigos
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; plej <tld/>a bovino donas lakton nur blankan
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="blank.0a">blanka</ref>
    <ref tip="vid" cel="griz.0a.koloro">griza</ref>
    <refgrp tip="vid">
      <ref cel="kest.nigra0o">nigra kesto</ref>,
      <ref cel="list.nigra_0o">nigra listo</ref>,
      <ref cel="tru.nigra0o">nigra truo</ref>
    </refgrp>
    <trd lng="id">hitam</trd>
    <trdgrp lng="tr">
      <trd>kara</trd>,
      <trd>siyah</trd>
    </trdgrp>
    <trd lng="vo">bl&auml;gik</trd>
  </snc>
  <snc mrk="nigr.0a.malluma">
    <dif>
      <ref tip="dif" cel="lum.mal0a">Malluma</ref>:
      <ekz>
        &ccirc;irka&ubreve; li fari&gcirc;is plej <tld/>a nokto
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili atingis la monton, sur kiu la <tld/>aj arbaroj levis sian supron al la
        &ccirc;ielo
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="arb.Nigra_A0aro">Nigra Arbaro</ref>,
      <ref cel="mar.NigraM0o">Nigra Maro</ref>
    </refgrp>
    <trdgrp lng="id">
      <trd>gelap</trd>,
      <trd>kelam</trd>
    </trdgrp>
  </snc>
  <snc mrk="nigr.0a.FIG_malgaja">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Funebra, <ref cel="gaj.mal0a">malgaja</ref>, pesimisma:
      <ekz>
	<ref cel="humur.nigra0o"><tld/>a humuro</ref>
        <fnt>
          <bib>ChC</bib>
          <lok>11</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a &ccirc;apitro de la historio
        <fnt>
          <bib>EeP</bib>
          <lok>Kvina Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a standardo de la morto
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        li komencis dronigadi siajn pensojn <klr>[&#x2026;]</klr> en brando,
        <klr>[kaj kiam]</klr> atakis lin lia <tld/>a spirito, <klr>[&#x2026;]</klr> li fari&gcirc;adis
        nekvietigebla kaj kuradis tra la arbaro kiel &ccirc;asata besto
        <fnt>
          <bib>Fab3</bib>
          <lok>Muta libro</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="roz.0a.FIG">roza<sncref/></ref>
    <refgrp tip="vid">
      <ref cel="griz.0a.sengaja">griza<sncref/></ref>,
      <ref cel="melank.0a">melankolia</ref>,
      <ref cel="vid.nigre0ulo"><tld/>evidulo</ref>
    </refgrp>
  </snc>
  <snc mrk="nigr.0a.malica">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malicega, abomeninda:
      <ekz>
        <ref cel="magi.nigra0o"><tld/>a magio</ref>
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz>
      <ekz>
	vi, <tld/>aj semantoj de malpaco
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado en la Guildhall (Londono) en la 21a de a&ubreve;gusto 1907</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="tr">kara</trd>
  </snc>
  <trd lng="af">swart</trd>
