<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>nigr</rad>/a
    </kap>

    <drv mrk="nigr.0a">
      <kap>
	<ofc>*</ofc><tld/>a
      </kap>
      <snc>
	<dif>
	  De koloro plej kontra&ubreve;a al blanko, samkolora kiel karbo kaj fulgo:
	  <ekz>
	    <tld/>a kiel inko, korva flugilo;
	  </ekz><ekz>
	    <tld/>a frako;
	  </ekz><ekz>
	    <tld/>e vestita;
	  </ekz><ekz>
	    karbo<tld/>a<fnt>Z</fnt>.
	  </ekz>
	</dif>
	<trdgrp lng="tr">
	  <trd>kara</trd>,
	  <trd>siyah</trd>
	</trdgrp>
      </snc>
      <snc>
	<dif>
	  Malluma:
	  <ekz>
	    <tld/>a nokto;
	  </ekz>
	</dif>
      </snc>
      <snc>
	<uzo tip="stl">FIG</uzo>
	<dif>
	  Funebra, malgaja, pesimisma:
	  <ekz>
	    <tld/>a humoro;
	  </ekz><ekz>
	    <tld/>a (profunda) mizero.
	  </ekz><ekz>
	    <tld/>aj ideoj
	    (<ref tip="sin" cel="melank.0o">melankolio</ref>);
	  </ekz>
	</dif>
	<refgrp tip="vid">
	  <ref cel="vid.nigre0ulo"><tld/>evidulo</ref>
	</refgrp>
      </snc>
      <snc>
	<uzo tip="stl">FIG</uzo>
	<dif>
	  Malicega, abomeninda:
	  <ekz>
	    <tld/>a animo, koro, krimo;
	  </ekz><ekz>
	    vi, <tld/>aj semantoj de malpaco<fnt>Z</fnt>.
	  </ekz>
	</dif>
	<trd lng="tr">kara</trd>
      </snc>
      <trd lng="cs">&ccaron;ern&yacute;</trd>
      <trd lng="en">black</trd>
      <trd lng="es">negro</trd>
      <trd lng="nl">zwart</trd>
      <trd lng="fr">noir</trd>
      <trd lng="de">schwarz</trd>
      <trd lng="pl">czarny</trd>
      <trd lng="ru">&c_ch;&c_jo;&c_r;&c_n;&c_y;&c_j;</trd>
      <trd lng="sv">svart</trd>
    </drv>

    <drv mrk="nigr.0o">
      <kap>
	<tld/>o
      </kap>
      <snc>
	<dif>
	  <tld lit="N"/>a koloro:
	  <ekz>
	    <tld/>o sur blanko (= skriba&jcirc;o) pruvas sen
	    manko<fnt>Z</fnt>.
	  </ekz>
	</dif>
      </snc>
      <trd lng="es">negro</trd>
      <trdgrp lng="fr">
	<trd>noir<klr> (subst. couleur)</klr></trd>
      </trdgrp>
      <trd lng="nl">zwart</trd>
      <trdgrp lng="ru">
	<trd>&c_ch;&c_je;&c_r;&c_n;&c_o;&c_t;&c_a;</trd>,
	<trd>&c_ch;&c_jo;&c_r;&c_n;&c_y;&c_j; &c_c;&c_v;&c_je;&c_t;</trd>
      </trdgrp>
    </drv>

    <drv mrk="nigr.0ajxo">
      <kap>
	<tld/>a&jcirc;o
      </kap>
      <snc>
	<dif>
	  <tld lit="N"/>a parto de io.
	</dif>
      </snc>
    </drv>

    <drv mrk="nigr.0igi">
      <kap>
	<tld/>igi
      </kap>
      <snc>
	<dif>
	  Fari <tld/>a:
	  <ekz>
	    <tld/>igi grizajn harojn.
	  </ekz>
	</dif>
	<trd lng="es">ennegrecer</trd>
	<trdgrp lng="fr">
	  <trd>noircir</trd>
	</trdgrp>
	<trd lng="nl">zwart maken</trd>
	<trd lng="ru">&c_ch;&c_je;&c_r;&c_n;&c_i;&c_t;&c_mol;</trd>
	<trd lng="tr">karalamak</trd>
      </snc>
      <snc>
	<uzo tip="stl">FIG</uzo>
	<dif>
	  Malgajigi:
	  <ekz>
	    la malsano <tld/>igis lian spiriton, vivon.
	  </ekz>
	</dif>
	<trdgrp lng="fr">
	  <trd>attrister</trd>,
	  <trd>noircir<klr> (attrister)</klr></trd>
	</trdgrp>
	<trd lng="ru">&c_o;&c_m;&c_r;&c_a;&c_ch;&c_i;&c_t;&c_mol;</trd>
      </snc>
      <snc>
	<uzo tip="stl">FIG</uzo>
	<dif>
	  Malbonfamigi:
	  <ekz>
	    kalumniante konstante, oni e&ccirc; an&gcirc;elojn
	    <tld/>igas<fnt>Z</fnt>;
	  </ekz><ekz>
	    <tld/>igi ies intencojn.
	  </ekz>
	</dif>
	<trdgrp lng="fr">
	  <trd>calomnier</trd>,
	  <trd>m&eacute;dire de</trd>,
	  <trd>noircir<klr> (calomnier)</klr></trd>
	</trdgrp>
	<trd lng="ru">&c_o;&c_ch;&c_je;&c_r;&c_n;&c_i;&c_t;&c_mol;</trd>
      </snc>
    </drv>

    <drv mrk="nigr.N0aMaro">
      <kap>
	<tld lit="N"/>a Maro
      </kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
	<dif>
	  Interna maro
	  <ref tip="malprt" cel="atlant.0o">Atlantika</ref>
	  inter E&ubreve;ropo kaj Azio, &ccirc;e la bordoj de
	  <ref cel="rus.R0ujo">Rusujo</ref>, Ukrainujo,
	  Romanujo,
	  <ref cel="bulgar.B0ujo">Bulgarujo</ref>,
	  <ref cel="turk.T0ujo">Turkujo</ref> kaj
	  <ref cel="kartvel.K0ujo">Kartvelujo</ref>.
	</dif>
      </snc>
      <trd lng="bg">&c_Ch;&c_je;&c_r;&c_n;&c_o; &c_m;&c_o;&c_r;&c_je;</trd>
      <trd lng="ru">&c_Ch;&c_jo;&c_r;&c_n;&c_o;&c_je; &c_m;&c_o;&c_r;&c_je;</trd>
      <trd lng="tr">Karadeniz</trd>
    </drv>


  </art>
