<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nigr.xml,v 1.46 2021/05/28 12:15:13 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>nigr</rad>/a <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="nigr.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="nigr.0a.KOL">
    <dif>
      De koloro plej kontra&ubreve;a al blanko, samkolora kiel karbo kaj
      fulgo:
      <ekz>
	<tld/>a kiel inko, korva flugilo;
      </ekz>
      <ekz>
	<tld/>a frako;
      </ekz>
      <ekz>
        sed post kelkaj tagoj li plene hejmi&gcirc;is inter la
        <ind>fulgo<tld/>a</ind>j viroj de la Torentaj uzinoj
         <fnt><aut>S. Engholm</aut>, <vrk>Al
         Torento</vrk>, <lok>1930</lok></fnt>;
        <trd lng="nl">gitzwart</trd>
      </ekz>
      <ekz>
	<tld/>e vestita;
      </ekz>
      <ekz>
	karbo<tld/>a<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="id">hitam</trd>
    <trdgrp lng="tr">
      <trd>kara</trd>,
      <trd>siyah</trd>
    </trdgrp>
    <trd lng="vo">bl&auml;gik</trd>
  </snc>
  <snc mrk="nigr.0a.malluma">
    <dif>
      Malluma:
      <ekz>
	<tld/>a nokto.
      </ekz>
    </dif>
    <trdgrp lng="id">
      <trd>gelap</trd>,
      <trd>kelam</trd>
    </trdgrp>
  </snc>
  <snc mrk="nigr.0a.FIG_malgaja">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Funebra, malgaja, pesimisma:
      <ekz>
	<tld/>a humoro;
      </ekz>
      <ekz>
	<ref tip="vid" cel="vid.nigre0ulo"><tld/>evidulo</ref>;
      </ekz>
      <ekz>
	<tld/>aj pensoj
	(<ref tip="vid" cel="melank.0o">melankolio</ref>).
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="griz.0a.sengaja">griza<sncref/></ref>,
      <ref cel="roz.0a.FIG">roza<sncref/></ref>
    </refgrp>
  </snc>
  <snc mrk="nigr.0a.malica">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malicega, abomeninda:
      <ekz>
	<tld/>a animo, koro, krimo;
      </ekz>
      <ekz>
	vi, <tld/>aj semantoj de malpaco<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="tr">kara</trd>
  </snc>
  <trd lng="be">&c_ch;&c_o;&c_r;&c_n;&c_y;</trd>
  <trd lng="bg">&c_ch;&c_je;&c_r;&c_je;&c_n;</trd>
  <trd lng="bo">&#xF53;&#xF42;&#xF0B;&#xF54;&#xF7C;</trd>
  <trd lng="br">du<klr tip="ind"> (ag.)</klr></trd>
  <trdgrp lng="cs">
    <trd>&ccaron;ern&yacute;</trd>,
    <trd>pokoutn&iacute;</trd>
  </trdgrp>
  <trd lng="de">schwarz</trd>
  <trd lng="el">&my;&alfa;&ypsilon_ton;&rho;&omikron;&sigma_fina;</trd>
  <trd lng="en">black</trd>
  <trd lng="es">negro</trd>
  <trd lng="fr">noir<klr> (adj.)</klr></trd>
  <trd lng="ja">&#x9ED2;&#x3044; [<ind>&#x304F;&#x308D;&#x3044;</ind>]</trd>
  <trd lng="nl">zwart<klr tip="ind"> (bn.)</klr></trd>
  <trd lng="pl">czarny</trd>
  <trdgrp lng="pt">
    <trd>negro</trd>,
    <trd>preto</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_jo;&c_r;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">pok&uacute;tny</trd>
  <trd lng="sv">svart</trd>
  <trd lng="tp">pimeja</trd>
  <trd lng="zh">&#x9ED1; [<ind>h&#x113;i</ind>]</trd>
</drv>

<drv mrk="nigr.0o">
  <kap><tld/>o</kap>
  <snc mrk="nigr.0o.KOL">
    <dif>
      <tld lit="N"/>a <ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">koloro</ref>:
      <ekz>
	<tld/>o sur blanko <klr>(= skriba&jcirc;o)</klr> pruvas sen
	manko
	<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="nigr.0o.ludanto">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Unu el la du ludantoj en ludoj kiel <ref cel="sxak.0o">&scirc;ako</ref>,
      <ref cel="go.0o">goo</ref>, <ref cel="dam.0oj0ludo">damoj</ref>,
      ktp.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_r;&c_n;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_ch;&c_o;&c_r;&c_n;&c_y; &c_k;&c_o;&c_l;&c_je;&c_r;</trd>
  </trdgrp>
  <trd lng="bg">&c_ch;&c_je;&c_r;&c_je;&c_n; &c_c;&c_v;&c_ja;&c_t;</trd>
  <trd lng="br">du<klr tip="ind"> (ak.)</klr></trd>
  <trdgrp lng="cs">
    <trd>&ccaron;ern&aacute; barva</trd>,
    <trd>&ccaron;er&ncaron;</trd>
  </trdgrp>
  <trd lng="de">Schw&auml;rze</trd>
  <trd lng="en">black</trd>
  <trd lng="es">negro<klr> (subst. color)</klr></trd>
  <trd lng="fr">noir<klr> (subst. couleur)</klr></trd>
  <trd lng="ja">&#x9ED2; [<ind>&#x304F;&#x308D;</ind>]</trd>
  <trd lng="ku">re&scedil;</trd>
  <trd lng="nl">zwart<klr tip="ind"> (zn.)</klr></trd>
  <trdgrp lng="pt">
    <trd>negro</trd>,
    <trd>preto</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_r;&c_n;&c_o;&c_t;&c_a;</trd>,
    <trd>&c_ch;&c_jo;&c_r;&c_n;&c_y;&c_j; &c_c;&c_v;&c_je;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">&ccaron;er&ncaron;</trd>
</drv>

<drv mrk="nigr.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="nigr.0ajxo.KOL">
    <dif>
      <tld lit="N"/>a parto de io.
    </dif>
  </snc>
  <trd lng="de">Schwarzes</trd>
  <trd lng="en">black</trd>
  <trd lng="es">negro</trd>
  <trd lng="nl">iets <ind>zwart</ind>s</trd>
</drv>

<drv mrk="nigr.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="nigr.0igi.KOL">
    <dif>
      Fari <tld/>a:
      <ekz>
	<tld/>igi grizajn harojn;
      </ekz>
      <ekz>
	<tld/>aj traboj transsaltis super niaj kapoj, kun 
        <ind>fum<tld/>igita</ind> plafono super ili
        <fnt>
          <aut>A. Giridhar Rao</aut>
          <vrk>La triumfo de rezono</vrk>
          <bib>Monato</bib>
          <lok>2001</lok>
        </fnt>.
        <trd lng="nl">zwartgeblakerd</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_ch;&c_a;&c_r;&c_n;&c_ib;&c_c;&c_mol;</trd>
    <trd lng="de">schw&auml;rzen</trd>
    <trd lng="en">blacken</trd>
    <trd lng="es">ennegrecer</trd>
    <trdgrp lng="fr">
      <trd>noircir</trd>
    </trdgrp>
    <trd lng="id">meng<ind>hitam</ind>kan</trd>
    <trd lng="nl"><ind>zwart</ind> maken</trd>
    <trd lng="ru">&c_ch;&c_je;&c_r;&c_n;&c_i;&c_t;&c_mol;</trd>
    <trd lng="tr">karalamak</trd>
  </snc>
  <snc mrk="nigr.0igi.FIG_malgajigi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malgajigi:
      <ekz>
	la malsano <tld/>igis lian spiriton, vivon.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_z;&c_m;&c_r;&c_o;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="de">verdunkeln</trd>
    <trd lng="es">entristecer</trd>
    <trdgrp lng="fr">
      <trd>attrister</trd>,
      <trd>noircir<klr> (attrister)</klr></trd>
    </trdgrp>
    <trd lng="nl">bedroeven</trd>
    <trd lng="ru">&c_o;&c_m;&c_r;&c_a;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="nigr.0igi.FIG_malbonfamigi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malbonfamigi:
      <ekz>
	kalumniante konstante, oni e&ccirc; an&gcirc;elojn
	<tld/>igas<fnt>Z</fnt>;
      </ekz>
      <ekz>
	<tld/>igi ies intencojn.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_ch;&c_a;&c_r;&c_n;&c_ja;&c_c;&c_mol;</trd>
    <trd lng="de">anschw&auml;rzen</trd>
    <trd lng="es">calumniar</trd>
    <trdgrp lng="fr">
      <trd>calomnier</trd>,
      <trd>m&eacute;dire de</trd>,
      <trd>noircir<klr> (calomnier)</klr></trd>
    </trdgrp>
    <trd lng="nl"><ind>zwart</ind> maken</trd>
    <trd lng="ru">&c_o;&c_ch;&c_je;&c_r;&c_n;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="br">dua&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>na&ccaron;ernit</trd>,
    <trd>o&ccaron;ernit</trd>,
    <trd>pomluvit</trd>,
    <trd>po&scaron;pinit</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>na&ccaron;ierni&tcaron;</trd>,
    <trd>o&ccaron;ierni&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="nigr.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="nigr.0ulo.ETN">
    <dif>
      Malhelha&ubreve;ta persono kun sudsahar-afrika deveno:
      <ekz>
        la bonhumora &ccirc;eesto de la <tld/>uloj vere mankis al mi poste
        <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro 10a, p. 34a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="negr.0o">negro</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_e;&c_g;&c_a;&c_r;</trd>,
    <trd>&c_ch;&c_a;&c_r;&c_n;&c_a;&c_s;&c_k;&c_u;&c_r;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_n;&c_je;&c_g;&c_malmol;&c_r;</trd>
  <trd lng="br">morian</trd>
  <trd lng="ca">negre <klr>(melanoderm)</klr></trd>
  <trdgrp lng="cs">
    <trd>negr</trd>,
    <trd>&ccaron;ernoch</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schwarzer</trd>,
    <trd>Neger</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>black person</trd>,
    <trd>black</trd>,
    <trd>negro</trd>
  </trdgrp>
  <trd lng="es">negro<klr> (subst. persona de color)</klr></trd>
  <trd lng="fa">&a_s;&f_y;&a_A;&a_h;&f_p;&a_w;&a_s;&a_t;</trd>
  <trdgrp lng="fr">
    <trd>noir<klr> (subst. personne)</klr></trd>,
    <trd>n&egrave;gre</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&kaf;&vav;&shin;&yod;</trd>,
    <trd>&shin;&het;&vav;&resh; <klr tip="ind">(&kaf;&vav;&shin;&yod;)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>n&eacute;ger</trd>,
    <trd>fekete</trd>
  </trdgrp>
  <trd lng="id">negro</trd>
  <trd lng="io">nigro</trd>
  <trd lng="ja">&#x9ED2;&#x4EBA;[<ind>&#x3053;&#x304F;&#x3058;&#x3093;</ind>]</trd>
  <trdgrp lng="nl">
    <trd>zwarte</trd>,
    <trd>neger</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>murzyn</trd>,
    <trd>czarnuch <klr>(obra&zacute;liwie)</klr></trd>
  </trdgrp>
  <trd lng="pt">negro</trd>
  <trd lng="ru">&c_n;&c_je;&c_g;&c_r;</trd>
  <trd lng="sk">&ccaron;ernoch</trd>
  <trd lng="sv">neger</trd>
  <trd lng="tr">zenci</trd>
</drv>

</art>
