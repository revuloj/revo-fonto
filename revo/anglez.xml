<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: anglez.xml,v 1.3 2023/05/10 09:26:13 afido Exp $">
    <kap>
      <rad>anglez</rad>/o <fnt><bib>BdV</bib></fnt>
    </kap>

<drv mrk="anglez.0o">
  <kap><tld/>o</kap>  
  <snc mrk="anglez.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Malnova vigla angla <ref tip="lst" lst="voko:dancoj" cel="danc.0o.MUZ">danco</ref> (17a &gcirc;is
      19a jc):
      <ekz>
        &gcirc;i ne estis kvadrilo, ne menueto kaj e&ccirc; ne simpla <tld/>o
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        varia&jcirc;oj <klr>[de kontra&ubreve;danco]</klr> estas ekzemple la kvadrilo, la <tld/>o, la
        ekosezo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=525315">Kontra&ubreve;danco</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Anglaise</trd>
    <trd lng="ru">&c_A;&c_n;&c_g;&c_l;&c_je;&c_z;</trd>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_g;&c_l;&c_je;&c_z;</trd>
  <trd lng="uk">&c_a;&#x43D;&c_g;&#x43B;&c_je;&c_z;&#xA0;(&c_t;&c_a;&#x43D;&c_je;&c_c;&#x44C;)</trd>
</drv>

  </art>

