    <trd>skate-board</trd>
  </trdgrp>
  <trd lng="hu">g&ouml;rdeszka</trd>
  <trd lng="id"><ind>papan</ind> luncur</trd>
  <trd lng="ja">&#x30B9;&#x30B1;&#x30FC;&#x30C8;&#x30DC;&#x30FC;&#x30C9;</trd>
  <trd lng="nl">skateboard</trd>
  <trd lng="pl">skateboard</trd>
