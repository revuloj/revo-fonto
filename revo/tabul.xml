    <uzo tip="fak">SPO</uzo>
    <dif>
      <tld lit="T"/>o kun radetoj, kiun oni surstaras, glitante sur
      glata grundo:
      <ekz>
        &scirc;ia filo ne rajtas uzi rul<tld/>on tie
        <fnt>
        <aut>russ</aut>,
        <vrk>
          <url ref="http://goulo.livejournal.com/2005/05/20/">
          damne, mi ne rajtas rulgliti sur bona strato</url></vrk>,
          <lok>russtaglibro, 2005-05-20</lok>
        </fnt>;
      </ekz>
      <ekz>
        same kiel rul<tld/>o, <klr>[&ccirc;e <ref tip="vid" cel="skut.0ilo">skutilo</ref>]</klr> oni devas
        treti por
        anta&ubreve;eniri, sed estas vertikala stango kun teniloj por pli
        facile gardi ekvilibron
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=211952">Skutilo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxu.rul0o.SPO">rul&scirc;uo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_je;&c_j;&c_t;&c_b;&c_o;&c_r;&c_d;</trd>
