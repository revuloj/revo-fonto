  <trdgrp lng="pl">
    <trd><ind>deska</ind> do surfowania</trd>,
    <trd><ind>deska</ind> surfingowa</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>prancha</trd>,
