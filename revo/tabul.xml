<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tabul</rad>/o
</kap>
<drv mrk="tabul.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tabul.0o.ligno">
    <dif>
      La&ubreve;longe segita, ne tre dika, plata peco el ligno:
      <ekz>
        kabano el <tld/>oj;
      </ekz>
      <ekz>
        transiri la rivereton per super&scirc;ovita <tld/>o;
      </ekz>
      <ekz>
        la instruisto skribis frazon sur la nigran <tld/>on;
      </ekz>
      <ekz>
        &scirc;ak<tld/>o, dam<tld/>o;
      </ekz>
      <ekz>
        kapti la lastan <tld/>on (&scirc;ancon) de savo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="plank.0o">planko</ref>,
      <ref cel="trab.0o">trabo</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="cel.0i">celi</ref>.
    </refgrp>
    <trdgrp lng="hu">
      <trd>deszka</trd>,
      <trd>t&aacute;bla</trd>
    </trdgrp>
  </snc>
  <snc mrk="tabul.0o.ne_ligno">
    <dif>
      Samforma peco el alia materio.
    </dif>
    <trd lng="hu">t&aacute;bla</trd>    
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_o;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Brett</trd>
  <trd lng="en">plank</trd>
  <trdgrp lng="fr">
    <trd>planche</trd>,
    <trd>panneau</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>plank</trd>,
    <trd>bord</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>br&auml;da</trd>,
    <trd>skiva</trd>,
    <trd>tavla</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="tabul.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Io farita el <tld/>oj:
      <ekz>
        <ind>mura sur<tld/>a&jcirc;o</ind>.
        <trdgrp lng="hu">
          <trd>falburkolat</trd>,
          <trd>falbor&iacute;t&aacute;s</trd>,
          <trd>lamb&eacute;ria</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>deszk&aacute;zat</trd>,
    <trd>t&aacute;blaburkolat</trd>,
    <trd>faburkolat</trd>
  </trdgrp>
</drv>

<drv mrk="tabul.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="tabul.0eto.eta">
    <dif>
      Maldika malgranda <tld/>o.
    </dif>
    <trd lng="hu">t&aacute;bl&aacute;cska</trd>    
  </snc>
  <snc mrk="tabul.0eto.skribebla">
    <dif>
      Maldika malgranda <tld/>o kovrita per vakso, sur kiu la antikvuloj
      skribis, uzante skrapstrekilon; skrib<tld/>eto:
      <ekz>
        skribu miajn moralordonojn sur la <tld/>eto de via
        koro<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="hu">&iacute;r&oacute;t&aacute;bla</trd>
  </snc>
  <trd lng="fr">tablette</trd>
  <trd lng="ru">&c_d;&c_o;&c_shch;&c_je;&c_ch;&c_k;&c_a;</trd>
</drv>
</art>
