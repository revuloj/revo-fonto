    <kap>
      <rad>Limo&gcirc;</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="limogx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Franca <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>, &ccirc;. 180&#8239;km nordoriente de
      Bordozo,
      fama pro porcelana&jcirc;oj kaj emajla&jcirc;oj:
      <ekz>
        se mi diras <tld/>o, ili eble malmultekostigos la aferon je ok tagoj, &ccirc;ar
        <tld/>o ja pli proksimas ol Parizo
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia avo <klr>[&#8230;]</klr> en Avinjono li parolis la <tld lit="l"/>an kaj oni
        respondis al li en la provenca dialekto
        <fnt>
          <bib>Monato</bib>
          <aut>Clair Mah&eacute;</aut>
          <vrk><url ref="&Monato;/2006/008904.php"
              >La okcitana lingvo</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Limoges</trd>
    <trd lng="en">Limoges</trd>
    <trd lng="fr">Limoges</trd>
    <trd lng="ru">&c_L;&c_i;&c_m;&c_o;&c_zh;</trd>
  </snc>
<trd lng="be">&c_L;&c_ib;&c_m;&c_o;&c_zh;</trd>
</drv>

