<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gvatemal</rad>/o
  <ofc>8</ofc>
</kap>

<drv mrk="gvatemal.0o">
  <kap>
    <tld lit="G"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="gvatemal.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la centra <ref tip="malprt"
      cel="amerik.A0o">Ameriko</ref> kun &ccirc;efurbo Gvatemalurbo.
      Teritorio de Respubliko Gvatemalo.
      ISO-3166 kodo: GT.
      &Gcirc;i limas kun
      <ref tip="vid" cel="beliz.0o.GEOG">Belizo</ref>,
      <ref tip="vid" cel="hondur.0o.GEOG">Honduro</ref>,
      <ref tip="vid" cel="meksik.0o.GEOG">Meksiko</ref>,
      <ref tip="vid" cel="salvador.0o.GEOG">Salvadoro</ref>
      kaj oceanoj:
      <ref tip="vid" cel="atlant.0o">Atlantiko</ref>
      kaj <ref tip="vid" cel="pacifik.0o">Pacifiko</ref>.
    </dif>
    <trdgrp lng="pl">
      <trd>Gwatemala</trd>,
      <trd>Republika <ind>Gwatemali</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

