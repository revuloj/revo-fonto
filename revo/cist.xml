<art mrk="$Id: cist.xml,v 1.15 2021/03/06 06:20:01 revo Exp $">
<kap>
  <rad>cistac</rad>/oj
</kap>
<drv mrk="cist.0oj">
  <kap>
    <tld/>oj
  </kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref>
      de <ref tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Cistaceae</trd>):
      <ekz>
        <ref tip="prt" cel="helian2.0o">heliantemo</ref> a&ubreve; sunrozo,
        estas genro de
        <klr>[&#x2026;]</klr> &ccirc;iamverdaj a&ubreve; <klr>[duon]</klr>-&ccirc;iamverdaj
        subarbedoj
        en la familio <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=417748">Heliantemo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">cist&agrave;cies</trd>
  <trd lng="de">Cistrosengew&auml;chse</trd>
  <trd lng="fa">&a_t;&f_y;&a_r;&a_h;&a_hamza_sure; <ind>&f_g;&a_l; &a_A_madda;&a_f;&a_t;&a_A;&a_b;&f_y;</ind></trd>
  <trd lng="fr">Cistac&eacute;es</trd>
  <trd lng="he">&#x5E6;&#x5D9;&#x5E1;&#x5D8;&#x5D5;&#x5EA;</trd>
  <trd lng="hu">napvir&aacute;gf&eacute;l&eacute;k</trd>
  <trd lng="it">cistacee</trd>
  <trd lng="pl">czystkowate</trd>
  <trd lng="pt">cist&aacute;ceas</trd>
  <trd lng="ru">&c_l;&c_a;&c_d;&c_a;&c_n;&c_n;&c_i;&c_k;&c_o;&c_v;&c_y;&c_je;
  <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
