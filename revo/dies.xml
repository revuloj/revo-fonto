<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>9</ofc><rad>dies</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="dies.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="dies.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Kromsigno altiganta noton je duonduto; simb. &#9839;.
    </dif>
    <trdgrp lng="de">
      <trd>Kreuz</trd>,
      <trd>Erh&ouml;hungszeichen</trd>
    </trdgrp>
    <trd lng="en">sharp</trd>
    <trd lng="es">sostenido</trd>
    <trd lng="hu">kereszt<klr tip="ind"> (zenei)</klr></trd>
    <trd lng="pl">krzy&zdot;yk</trd>
    <trdgrp lng="pt">
      <trd>sustenido</trd>,
      <trd>diese</trd>
    </trdgrp>
  </snc>
  <snc mrk="dies.0o.komp">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <ref tip="dif" cel="krad.0o.komp">Krado</ref>
      <klr>(la signo # de Askio)</klr>.
    </dif>
    <rim>
      Fakte inter la Askia krado # kaj la muzika <tld/>o &#9839;
      estas rimarkebla diferenco grafika, kaj en bona tipografio
      kaj en Unikodo ili estas malsamaj signoj.
    </rim>
    <trdgrp lng="de">
      <trd>Lattenzaun</trd>,
      <trd>Nummernzeichen</trd>,
      <trd>Raute(taste) [sur mobiltelefonoj]</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>number sign</trd>, <trd>hash</trd>, <trd>pound sign</trd>,
      <trd>grid</trd>, <trd>octothorpe</trd>, <trd>sharp</trd>,
    </trdgrp>    <trdgrp lng="pl">
      <trd>krzy&zdot;yk</trd>,
      <trd>kratka<klr> (pot.)</klr></trd>,
      <trd>p&lstroke;otek<klr> (pot.)</klr></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>cerquilha</trd>,
      <trd>tralha</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_je;&c_z;</trd>
  <trd lng="ca">coixinet</trd>
  <trd lng="es">almohadilla</trd>
  <trd lng="fr">di&egrave;se</trd>
  <trd lng="hu">sz&aacute;mjel</trd>
  <trd lng="ru">&c_d;&c_i;&c_je;&c_z;</trd>
  <trd lng="sk">kr&iacute;&zcaron;ik <klr>(hudobn&yacute;)</klr></trd>
</drv>

<drv mrk="dies.du0o">
  <kap>du<tld/>o</kap>
  <snc mrk="dies.du0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Kromsigno altiganta noton je duto.
    </dif>
    <trd lng="en">double <ind>sharp</ind></trd>
  </snc>
  <trd lng="de">Doppelkreuz</trd>
  <trd lng="es">doble <ind>sostenido</ind></trd>
  <trd lng="fr">double <ind>di&egrave;se</ind></trd>
  <trd lng="hu">kett&odblac;s <ind>kereszt</ind></trd>
  <trd lng="pl">podw&oacute;jny <ind>krzy&zdot;yk</ind></trd>
  <trd lng="sk">dvojit&yacute; <ind>kr&iacute;&zcaron;ik</ind></trd>
</drv>
</art>
