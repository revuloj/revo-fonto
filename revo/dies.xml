<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>dies</rad>/o
    </kap>
    <drv mrk="dies.0o">
      <kap><tld/>o</kap>
      <snc mrk="dies.0o.muz">
        <uzo tip="fak">MUZ</uzo>
        <dif>
          Kromsigno duontone altiganta la noton.
        </dif>
      </snc>
      <snc mrk="dies.0o.komp">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          <ref tip="dif" cel="krad.0o.komp">Krado</ref>
          <klr>(la signo # de Askio)</klr>.
        </dif>
        <rim>
          Fakte inter la Askia krado # kaj la muzika <tld/>o &#9839;
          estas rimarkebla diferenco grafika, kaj en bona tipografio
          kaj en Unikodo ili estas malsamaj signoj.
        </rim>
      </snc>
      <trd lng="en">sharp</trd>
      <trd lng="ru">&#x434;&#x438;&#x435;&#x437;</trd>
    </drv>
    <drv mrk="dies.du0o">
      <kap>du<tld/>o</kap>
      <uzo tip="fak">MUZ</uzo>
      <snc>
        <dif>
          Kromsigno je unu tono altiganta la noton.
        </dif>
      </snc>
    </drv>
  </art>
<!--
