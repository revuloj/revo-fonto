<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: geneal.xml,v 1.34 2025/10/01 02:12:49 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>genealogi</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="geneal.0o">
  <kap><tld/>o</kap>
  <snc mrk="geneal.0o.SCI">
    <uzo tip="fak">GEN</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Scienco esploranta devenon kaj parencecon de familioj, gentoj
      homaj a&ubreve; bestaj:
      <ekz>
        nuntempe <tld/>o estas &scirc;atokupo de fervoraj amatoroj
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="https://www.monato.be/2006/008970.php">Genealogio</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Ahnenforschung</trd>,
      <trd>Familienforschung</trd>
    </trdgrp>
    <trd lng="fa">&a_t;&a_b;&a_A;&a_r;&a_sh;&a_n;&a_A;&a_s;&f_y;</trd>
    <trdgrp lng="hu">
      <trd>sz&aacute;rmaz&aacute;stan</trd>,
      <trd>csal&aacute;dfa</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>genealogi</trd>,
      <trd>ilmu nasab</trd>
    </trdgrp>
    <trd lng="pl">genealogia</trd>
    <trd lng="pt">genealogia</trd>
    <trd lng="ru">&c_g;&c_je;&c_n;&c_je;&c_a;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  </snc>
  <snc mrk="geneal.0o.dokumento">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="super" cel="regist.0o">Registro</ref> pri <ref cel="ven.de0o">deveno</ref> kaj
      parenceco de gentanoj:
      <ekz>
        jen estas la <tld/>o de <nom>Aaron</nom> kaj Moseo en tiu tempo <klr>[&#8230;]</klr>
        <fnt><bib>MT</bib> <lok>&Nom; 3:1</lok></fnt>;
      </ekz><ekz>
        kun la persona komputilo anka&ubreve; venis programoj por fari <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="https://www.monato.be/2006/008970.php">Genealogio</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Abstammung</trd>,
      <trd>Stammbaum</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_sh;&a_j;&a_r;&a_h;</trd>,
      <trd>&#x200C;&a_sh;&a_j;&a_r;&a_h;&#x200C;&a_n;&a_A;&a_m;&a_h;</trd>,
      <trd>&a_n;&a_s;&a_b;&#x200C;&a_n;&a_A;&a_m;&a_h;</trd>
    </trdgrp>
    <trd lng="hu">csal&aacute;dfa</trd>
    <trd lng="id">silsilah</trd>
    <trd lng="pl">drzewo <ind>genealogiczne</ind></trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_o;&c_d;&c_o;&c_s;&c_l;&c_o;&c_v;&c_n;&c_a;&c_ja;</trd>,
      <trd>&c_r;&c_o;&c_d;&c_o;&c_s;&c_l;&c_o;&c_v;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_je;&c_n;&c_e;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_d;&c_a;&c_v;&c_o;&c_d;</trd>
  </trdgrp>
  <trd lng="bg">&c_g;&c_je;&c_n;&c_je;&c_a;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="ca">genealogia</trd>
  <trdgrp lng="cs">
    <trd>genealogie</trd>,
    <trd>rodopis</trd>
  </trdgrp>
  <trd lng="de">Genealogie</trd>
  <trd lng="en">genealogy</trd>
  <trd lng="fr">g&eacute;n&eacute;alogie</trd>
  <trdgrp lng="ja">
    <trd>&#x7CFB;&#x56F3;&#x5B66;  <pr>&#12369;&#12356;&#x305A;&#x304C;&#x304F;</pr></trd>,
    <trd>&#x7CFB;&#x56F3;  <pr>&#12369;&#12356;&#x305A;</pr></trd>,
    <trd>&#x5BB6;&#x7CFB;  <pr>&#x304B;&#12369;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>afstamming</trd>,
    <trd>genealogie</trd>
  </trdgrp>
  <trd lng="pt">genealogia</trd>
  <trdgrp lng="sk">
    <trd>rodokme&ncaron;</trd>,
    <trd>rodopis</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>soybilim</trd>,
    <trd>jeneoloji</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_je;&c_n;&c_je;&c_a;&c_l;&c_o;&c_g;&c_ib;&c_ja;</trd>,
    <trd>&c_r;&c_o;&c_d;&c_o;&c_v;&c_ib;&c_d;</trd>
  </trdgrp>
  <trd lng="zh">&#x7CFB;&#x8B5C; <pr>x&#xEC;p&#x1D4;</pr></trd>
</drv>

<drv mrk="geneal.0a">
  <kap><tld/>a</kap>
  <snc mrk="geneal.0a.SCI">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        en lia <ref cel="arb.genealogia0o.GEN"><tld/>a arbo</ref> trovi&gcirc;is multe
        pli da Parlamentaj konsilantoj ol da
        partoprenantoj de la <nom>Versailles</nom>&#8217;a kortego
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>;
      </ekz><ekz>
        li ne estis enskribita en la <tld/>an liston kiel unuenaskito
        <fnt><bib>MT</bib> <lok>&Kr1; 5:1</lok></fnt>;
      </ekz><ekz>
        utilas komence ali&gcirc;i al <tld/>a asocio por alkutimi&gcirc;i al esplormetodoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="https://www.monato.be/2006/008970.php">Genealogio</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        la senco de la lingva <tld/>a arbo estis montri, ke la homaro havis iam unu
        solan pralingvon, kiu poste (pro nekonataj ka&ubreve;zoj) disbran&ccirc;i&gcirc;is
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_n;&c_e;&c_a;&c_l;&c_ja;&c_g;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="bg">&c_g;&c_je;&c_n;&c_je;&c_a;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_n;</trd>
  <trd lng="ca">geneal&ograve;gic</trd>
  <trd lng="cs">rodopisn&yacute;</trd>
  <trd lng="de">genealogisch</trd>
  <trd lng="en">genealogical</trd>
  <trd lng="fa">&a_t;&a_b;&a_A;&a_r;&a_sh;&a_n;&a_A;&a_kh;&a_t;&f_y;</trd>
  <trd lng="fr">g&eacute;n&eacute;alogique</trd>
  <trdgrp lng="hu">
    <trd>sz&aacute;rmaz&aacute;si</trd>,
    <trd>geneal&oacute;giai</trd>
  </trdgrp>
  <trd lng="id">genealogis</trd>
  <trdgrp lng="ja">
    <trd>&#x7CFB;&#x56F3;&#x306E;  <pr>&#12369;&#12356;&#x305A;&#x306E;</pr></trd>,
    <trd>&#x5BB6;&#x7CFB;&#x306E;  <pr>&#x304B;&#12369;&#12356;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">genealogisch</trd>
  <trd lng="pt">geneal&oacute;gico<klr tip="ind"> (adj.)</klr></trd>
   <trdgrp lng="ru">
    <trd>&c_g;&c_je;&c_n;&c_je;&c_a;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_r;&c_o;&c_d;&c_o;&c_s;&c_l;&c_o;&c_v;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">rodopisn&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_je;&c_n;&c_je;&c_a;&c_l;&c_o;&c_g;&c_ib;&c_ch;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_r;&c_o;&c_d;&c_o;&c_v;&c_ib;&c_d;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5B97;&#x7CFB; <pr>z&#x14D;ngx&#xEC;</pr></trd>,
    <trd>&#x5BB6;&#x7CFB; <pr>ji&amacron;x&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="geneal.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="geneal.0isto.SCI">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Fakulo pri <ref tip="vid" cel="geneal.0o.SCI"><tld/>o</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_d;&c_a;&c_v;&c_o;&c_d;&c_c;&c_a;</trd>
  <trd lng="bg">&c_g;&c_je;&c_n;&c_je;&c_a;&c_l;&c_o;&c_g;</trd>
  <trd lng="ca">genealogista</trd>
  <trd lng="de">Genealog</trd>
  <trd lng="en">genealogist</trd>
  <trd lng="fa">&a_t;&a_b;&a_A;&a_r;&a_sh;&a_n;&a_A;&a_s;</trd>
  <trd lng="fr">g&eacute;n&eacute;alogiste</trd>
  <trdgrp lng="id">
    <trd>ahli ilmu nasab</trd>,
    <trd>genealog</trd>
  </trdgrp>
  <trd lng="nl">genealoog</trd>
  <trd lng="pl">genealog</trd>
  <trd lng="ru">&c_s;&c_p;&c_je;&c_c;&c_i;&c_a;&c_l;&c_i;&c_s;&c_t; &c_p;&c_o; <ind>&c_g;&c_je;&c_n;&c_je;&c_a;&c_l;&c_o;&c_g;&c_i;&c_i;</ind></trd></drv>
</art>
