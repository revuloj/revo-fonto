<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>genealogi</rad>/o
</kap>
<drv mrk="geneal.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Tabelo de deveno k parenceco de la membroj de gento:
      <ekz>fari, starigi la <tld/>on de familio;</ekz>
      <ekz>la <tld/>o de David, de ras&ccirc;evalo.</ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Genealogie</trd>,
     <trd>Abstammung</trd></trdgrp>
  <trd lng="fr">g&eacute;n&eacute;alogie</trd>
  <trd lng="ru">&c_g;&c_je;&c_n;&c_je;&c_a;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
</drv>
<drv mrk="geneal.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <ind><tld/>a arbo</ind>
        <klr>
          (kies trunko estas la unuaj gepatroj, k bran&ccirc;oj la posteuloj)
        </klr>.
        <trd lng="de">Stammbaum</trd>
        <trd lng="fr">arbre g&eacute;n&eacute;alogique</trd>
        <trd lng="ru">&c_g;&c_je;&c_n;&c_je;&c_a;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; &c_d;&c_r;&c_je;&c_v;&c_o;</trd>
      </ekz>  
    </dif>
  </snc>
  <trd lng="fr">g&eacute;n&eacute;alogique</trd>
  <trd lng="ru">&c_g;&c_je;&c_n;&c_je;&c_a;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
