<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>genealogi</rad>/o
</kap>
<drv mrk="geneal.0o">
  <kap><tld/>o</kap>
  <snc mrk="geneal.0o.SCI">
    <uzo tip="fak">GEN</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Scienco esploranta devenon kaj parencecon de familioj, gentoj
      homaj a&ubreve; bestaj.
    </dif>
    <trd lng="fa">&a_t;&a_b;&a_A;&a_r;&a_sh;&a_n;&a_A;&a_s;&f_y;</trd>
    <trdgrp lng="hu">
      <trd>sz&aacute;rmaz&aacute;stan</trd>,
      <trd>csal&aacute;dfa</trd>
    </trdgrp>
    <trd lng="pt">genealogia</trd>
  </snc>
  <snc mrk="geneal.0o.dokumento">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Tabelo a&ubreve; listo pri deveno kaj parenceco de gentanoj:
      <ekz>
        fari, starigi la <tld/>on de familio;
      </ekz>
      <ekz>
        la <tld/>o de David, de ras&ccirc;evalo.
      </ekz>
    </dif>
    <trdgrp lng="fa">
      <trd>&a_sh;&a_j;&a_r;&a_h;</trd>,
      <trd>&zwnj;&a_sh;&a_j;&a_r;&a_h;&zwnj;&a_n;&a_A;&a_m;&a_h;</trd>,
      <trd>&a_n;&a_s;&a_b;&zwnj;&a_n;&a_A;&a_m;&a_h;</trd>
    </trdgrp>
    <trd lng="hu">csal&aacute;dfa</trd>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_n;&c_e;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_g;&c_je;&c_n;&c_je;&c_a;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="ca">genealogia</trd>
  <trdgrp lng="de">
    <trd>Genealogie</trd>,
    <trd>Abstammung</trd>
  </trdgrp>
  <trd lng="fr">g&eacute;n&eacute;alogie</trd>
  <trdgrp lng="nl">
    <trd>afstamming</trd>,
    <trd>genealogie</trd>
  </trdgrp>
  <trd lng="pt">genealogia</trd>
  <trd lng="ru">&c_g;&c_je;&c_n;&c_je;&c_a;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
</drv>

<drv mrk="geneal.0a">
  <kap><tld/>a</kap>
  <snc mrk="geneal.0a.SCI">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <ind><tld/>a arbo</ind>
        <klr>
        (kies trunko estas la unuaj gepatroj, kaj bran&ccirc;oj la posteuloj)
        </klr>.
        <trd lng="be">&c_g;&c_je;&c_n;&c_e;&c_a;&c_l;&c_ja;&c_g;&c_ib;&c_ch;&c_n;&c_a;&c_je; &c_d;&c_r;&c_e;&c_v;&c_a;</trd>
        <trd lng="bg">&c_r;&c_o;&c_d;&c_o;&c_s;&c_l;&c_o;&c_v;&c_n;&c_o; &c_d;&c_malmol;&c_r;&c_v;&c_o;</trd>
        <trd lng="ca">arbre <ind>geneal&ograve;gic</ind></trd>
        <trd lng="de">Stammbaum</trd>
        <trdgrp lng="fa">
          <trd>&a_sh;&a_j;&a_r;&a_h;</trd>,
          <trd>&zwnj;&a_sh;&a_j;&a_r;&a_h;&zwnj;&a_n;&a_A;&a_m;&a_h;</trd>,
          <trd>&a_n;&a_s;&a_b;&zwnj;&a_n;&a_A;&a_m;&a_h;</trd>
        </trdgrp>
        <trd lng="fr">arbre g&eacute;n&eacute;alogique</trd>
        <trd lng="hu">csal&aacute;dfa</trd>
        <trd lng="nl">stamboom</trd>
        <trd lng="pt">&aacute;rvore geneal&oacute;gica</trd>
        <trd lng="ru">&c_g;&c_je;&c_n;&c_je;&c_a;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je;
        &c_d;&c_r;&c_je;&c_v;&c_o;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_n;&c_e;&c_a;&c_l;&c_ja;&c_g;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="fa">&a_t;&a_b;&a_A;&a_r;&a_sh;&a_n;&a_A;&a_kh;&a_t;&f_y;</trd>
  <trd lng="fr">g&eacute;n&eacute;alogique</trd>
  <trdgrp lng="hu">
    <trd>sz&aacute;rmaz&aacute;si</trd>,
    <trd>geneal&oacute;giai</trd>
  </trdgrp>
  <trd lng="nl">genealogisch</trd>
  <trd lng="pt">geneal&oacute;gico<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="ru">&c_g;&c_je;&c_n;&c_je;&c_a;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="geneal.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="geneal.0isto.SCI">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Fakulo pri <ref tip="vid" cel="geneal.0o.SCI"><tld/>o</ref>.
    </dif>
  </snc>
  <trd lng="fa">&a_t;&a_b;&a_A;&a_r;&a_sh;&a_n;&a_A;&a_s;</trd>
  <trd lng="fr">g&eacute;n&eacute;alogiste</trd>
</drv>
</art>
