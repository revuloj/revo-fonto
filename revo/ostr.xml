<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>ostr</rad>/o
</kap>
<drv mrk="ostr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="ostr.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Ostrea</trd>), man&gcirc;ebla konko.
    </dif>
  </snc>
  <trd lng="de">Auster</trd>
  <trd lng="fr">hu&icirc;tre</trd>
</drv>
<drv mrk="ostr.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo
      <ref tip="super" cel="brank.faden0uloj">fadenbrankuloj</ref>
      (<trd lng="la">Ostreidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Austern <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>
