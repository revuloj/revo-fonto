<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>9</ofc><rad>pi&jcirc;am</rad>/o
</kap>

<drv mrk="pijxam.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Negli&gcirc;a a&ubreve; lita dupeca vesto kun ne al&gcirc;ustigita pantalono por
      viroj
      kaj virinoj.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_zh;&c_a;&c_m;&c_a;</trd>
  <trd lng="bg">&c_p;&c_i;&c_zh;&c_a;&c_m;&c_a;</trd>
  <trd lng="ca">pijama</trd>
  <trd lng="de">Schlafanzug</trd>
  <trdgrp lng="en">
    <trd>pajama</trd>, <trd>pajamas</trd>,
    <trd>pyjama</trd>, <trd>pyjamas</trd>
  </trdgrp>
  <trd lng="es">pijama</trd>
  <trd lng="fr">pyjama</trd>
  <trd lng="hu">pizsama</trd>
  <trd lng="nl">pijama</trd>
  <trd lng="pt">pijama</trd>
  <trd lng="ru">&c_p;&c_i;&c_zh;&c_a;&c_m;&c_a;</trd>
  <trd lng="sv">pyjamas</trd>
</drv>
</art>
