<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: orf.xml,v 1.34 2023/09/03 07:12:35 revo Exp $">
<kap>
  <ofc>*</ofc> <rad>orf</rad>/o
</kap>
<drv mrk="orf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="orf.0o.GEN">
  <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="super" cel="infan.0o">Infano</ref>, kies gepatroj a&ubreve; unu el ili mortis:
      <ekz>
        vidvinon kaj <tld/>on ne premu
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 22:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi estas kiel <tld/>o, vane ser&ccirc;anta vojon en la dezerto
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        komenci&gcirc;is malfacila tempo por la malfeli&ccirc;a <tld/>ino
        <fnt>
          <bib>ElektFab</bib>
          <lok>Cindrulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        Francisko <klr>[&#x2026;]</klr> proponas al malri&ccirc;a senhelpa <tld/>ino sian koron, sian
        manon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        de edzi&gcirc;o tro malfrua <tld/>oj naski&gcirc;as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        liaj laboriloj ku&scirc;as <tld/>e <klr>(forlasite)</klr> sur la
        stablo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_ib;&c_r;&c_a;&c_t;&c_a;</trd>
  <trd lng="bg">&c_s;&c_i;&c_r;&c_a;&c_k;</trd>
  <trd lng="br">emzivad</trd>
  <trd lng="cs">sirotek</trd>
  <trdgrp lng="de">
    <trd>Waisenkind</trd>,
    <trd>Waise</trd>
  </trdgrp>
  <trd lng="el">&omikron;&rho;&phi;&alfa;&ny;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">orphan</trd>
  <trd lng="es">hu&eacute;rfano</trd>
  <trd lng="fr">orphelin<klr tip="amb"> (subst.)</klr></trd>
  <trdgrp lng="fy">
    <trd>wees</trd>,
    <trd>weesbern</trd>
  </trdgrp>
  <trd lng="he">&yod;&tav;&vav;&fmem;</trd>
  <trd lng="hu">&aacute;rva</trd>
  <trd lng="id">yatim piatu</trd>
  <trd lng="io">orfano</trd>
  <trdgrp lng="ja">
    <trd>&#x5B64;&#x5150;  [<ind>&#x3053;&#x3058;</ind>]</trd>,
    <trd>&#x307F;&#x306A;&#x3057;&#x3054;</trd>,
    <trd>&#x7247;&#x89AA;&#x306E;&#x306A;&#x3044;&#x5B50;  [<ind>&#x304B;&#x305F;&#x3057;&#x3093;&#x306E;&#x306A;&#x3044;&#x3053;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>wees</trd>,
    <trd>weeskind</trd>
  </trdgrp>
  <trd lng="pl">sierota</trd>
  <trd lng="pt">&oacute;rf&atilde;o</trd>
  <trd lng="ru">&c_s;&c_i;&c_r;&c_o;&c_t;&c_a;</trd>
  <trd lng="sk">sirota</trd>
  <trd lng="sv">f&ouml;r&auml;ldral&ouml;s</trd>
  <trdgrp lng="tr">
    <trd>yetim</trd>,
    <trd>&ouml;ks&uuml;z</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_i;&c_r;&#x43E;&c_t;&c_a; (&c_h;&#x43B;&#x43E;&#x43F;&c_je;&c_c;&#x44C;)</trd>
  <trd lng="vo">nenpalan</trd>
</drv>

<drv mrk="orf.0a">
  <kap>
    <tld/>a
  </kap>
  <snc mrk="orf.0a.sengepatra">
    <dif>
      Sengepatra, senpatrina a&ubreve; senpatra:
      <ekz>
        <tld/>a tro frue, li heredis, en sia plena&gcirc;o, preska&ubreve; unu milionon
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        Josefino trovi&gcirc;is samtempe <tld/>ina kaj tute ruinigita
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili sentis sin kiel <tld/>aj gefratoj, forlasitaj kaj sen&scirc;irmaj
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sur vin hodia&ubreve; falas la devo zorgi pri la <tld/>a &scirc;tato
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">verwaist</trd>
  <trdgrp lng="ja">
    <trd>&#x5B64;&#x5150;&#x306E;  [<ind>&#x3053;&#x3058;&#x306E;</ind>]</trd>,
    <trd>&#x89AA;&#x306E;&#x306A;&#x3044;  [<ind>&#x3057;&#x3093;&#x306E;&#x306A;&#x3044;</ind>]</trd>,
    <trd>&#x898B;&#x6368;&#x3066;&#x3089;&#x308C;&#x305F;  [<ind>&#x307F;&#x3059;&#x3066;&#x3089;&#x308C;&#x305F;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&c_r;&c_ib;&c_t;&c_s;&#x44C;&#x43A;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_i;&c_r;&#x43E;&c_t;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="orf.0e">
  <kap>
    <tld/>e
  </kap>
  <snc mrk="orf.0e.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref cel="las.for0i">Forlasite</ref>:
      <ekz>
        Jam ne sonas via batalilo, // <tld lit="O"/>e ku&scirc;os ruste la armilo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        rondaj okuloj, inter kiuj <tld/>e sin levis malgranda ru&gcirc;a nazeto
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">verwaist</trd>
  <trd lng="uk">&c_s;&c_i;&c_r;&#x43E;&c_t;&#x43B;&c_i;&c_v;&#x43E;</trd>
</drv>


<drv mrk="orf.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="orf.0ejo.SAN">
    <dif>
      Rifu&gcirc;a <ref tip="super" cel="establ.0ejo">establejo</ref>, kie oni prizorgas <tld/>ojn:
      <ekz>
        la <tld/>ejo Sankta Jozefo en Havano
        <fnt>
          <bib>Monato</bib>
          <aut>Alberto Garc&iacute;a Fumero</aut>
          <vrk><url ref="&Monato;/2009/009702.php">Frato Jos&eacute; Olallo Vald&eacute;s beata</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>ejo<klr>[n&#x2026;]</klr> li dum 30 jaroj gvidis
        <fnt><aut>Zofia Banet-Fornalowa</aut>,
        <vrk><url ref="&Monato;/2000/006940.php">Korczak ridetanta</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/07, p. 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        la knabo estis sepjara, frekventis lernejon <klr>[&#x2026;]</klr> kiel la
        pluraj infanoj en la <tld/>ejo
        <fnt>
          <bib>Monato</bib>
          <aut>Julian Modest</aut>
          <vrk><url ref="&Monato;/2007/009024.php">Bela son&gcirc;o</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        la hospitalo konfidos la bebon al <tld/>ejo a&ubreve; ser&ccirc;os adoptonton
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo/pg</aut>
          <vrk><url ref="&Monato;/2007/009168.php">Lulilo de cikonioj</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_ib;&c_c;&c_ja;&c_ch;&c_y; &c_d;&c_o;&c_m;</trd>
  <trd lng="bg">&c_s;&c_i;&c_r;&c_o;&c_p;&c_i;&c_t;&c_a;&c_l;&c_i;&c_shch;&c_je;</trd>
  <trd lng="br">ti-emzivaded</trd>
  <trd lng="cs">sirot&ccaron;inec</trd>
  <trd lng="de">Waisenhaus</trd>
  <trd lng="el">&omikron;&rho;&phi;&alfa;&ny;&omikron;&tau;&rho;&omikron;&phi;&epsilon;&jota_ton;&omikron;</trd>
  <trd lng="en">orphanage</trd>
  <trd lng="es">orfanato</trd>
  <trd lng="fr">orphelinat</trd>
  <trd lng="fy">weesh&ucirc;s</trd>
  <trd lng="he">&bet;&yod;&tav; &yod;&tav;&vav;&mem;&yod;&fmem;</trd>
  <trd lng="hu">&aacute;rvah&aacute;z</trd>
  <trd lng="id"><ind>panti</ind> asuhan</trd>
  <trd lng="io">orfanerio</trd>
  <trd lng="ja">&#x5B64;&#x5150;&#x9662;  [<ind>&#x3053;&#x3058;&#x3044;&#x3093;</ind>]</trd>
  <trd lng="nl">weeshuis</trd>
  <trdgrp lng="pl">
    <trd>sierociniec <klr>(przest.)</klr></trd>,
    <trd>dom <ind>dziecka</ind></trd>,
    <trd>bidul <klr>(pot.)</klr></trd>
  </trdgrp>
  <trd lng="pt">orfanato</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_t;&c_s;&c_k;&c_i;&c_j; &c_d;&c_o;&c_m;</trd>,
    <trd>&c_s;&c_i;&c_r;&c_o;&c_t;&c_s;&c_k;&c_i;&c_j; &c_p;&c_r;&c_i;&c_ju;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">sirotinec</trd>
  <trd lng="sv">barnhem</trd>
  <trdgrp lng="tr">
    <trd>yetimhane</trd>,
    <trd>&ouml;ks&uuml;zler yurdu</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&c_r;&#x43E;&c_t;&c_i;&#x43D;&c_je;&c_c;&#x44C;</trd>,
    <trd>&c_d;&c_i;&c_t;&c_b;&c_u;&c_d;&c_i;&#x43D;&#x43E;&#x43A;</trd>,
    <trd>&#x43F;&c_r;&c_i;&c_t;&c_u;&#x43B;&#x43E;&#x43A; &c_d;&#x43B;&#x44F; &c_s;&c_i;&c_r;&c_ib;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="orf.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="orf.0igi.senpatrigi">
    <dif>
      <ref tip="super" cel="sen.0igi">Senigi</ref> infanon de patro a&ubreve; patrino:
      <ekz>
        nur sep viroj travivis la eksplodon, kiu <tld/>igis 162 infanojn
        <fnt><aut>Araneo</aut>,
        <vrk><url ref="&Monato;/2000/006949.php">Subtera katastrofo</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/10, p. 12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="orf.0igi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="las.for0i">Forlasi</ref> simile kiel mortanta patro a&ubreve; 
      patrino zorgaton a&ubreve; zorgata&jcirc;on:
      <ekz>
        ne bezonu mi <klr>[&#x2026;]</klr> levi mian manon kontra&ubreve; mi mem kaj en floranta a&gcirc;o
        <tld/>igi mian domon
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        miloj de la tagoj // Kaj de la noktoj, kiam <tld/>igita // Animo vane vokas la
        perditon
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">emzivada&ntilde;</trd>
  <trd lng="cs">u&ccaron;init sirotou</trd>
  <trd lng="de">zum <ind>Waisen</ind> machen</trd>
  <trd lng="en">orphan</trd>
  <trd lng="es">quedar hu&eacute;rfano</trd>
  <trd lng="fr">rendre <ind>orphelin</ind></trd>
  <trd lng="io">orfanigar</trd>
  <trdgrp lng="ja">
    <trd>&#x5B64;&#x5150;&#x306B;&#x3059;&#x308B;  [<ind>&#x3053;&#x3058;&#x306B;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x89AA;&#x3092;&#x596A;&#x3046;  [<ind>&#x3057;&#x3093;&#x3092;&#x3046;&#x3070;&#x3046;</ind>]</trd>
  </trdgrp>
  <trd lng="nl"><ind>wees</ind> maken</trd>
  <trd lng="pl">osieroci&cacute;</trd>
  <trd lng="pt">orfanar</trd>
  <trd lng="sk">urobi&tcaron; sirotou</trd>
  <trdgrp lng="tr">
    <trd>yetim b&inodot;rakmak</trd>,
    <trd>&ouml;ks&uuml;z b&inodot;rakmak</trd>
  </trdgrp>
  <trd lng="uk">&#x43E;&c_s;&c_i;&c_r;&#x43E;&c_t;&c_i;&c_t;&c_i;</trd>
</drv>
</art>
