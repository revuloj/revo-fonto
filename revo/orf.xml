<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc> <rad>orf</rad>/o</kap>
<drv mrk="orf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Knabo, kies patro a&ubreve; patrino mortis:
      <ekz>
        de edzi&gcirc;o tro malfrua <tld/>oj naski&gcirc;as<fnt>Z</fnt>;
      </ekz>
      (f):
      <ekz>
        tagoj kaj noktoj, kiam <tld/>igita animo vane vokas la
        perditon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        liaj laboriloj ku&scirc;as <tld/>e (forlasite) sur la stablo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Waisenkind</trd>,
    <trd>Waise</trd>
  </trdgrp>
  <trd lng="hu">&aacute;rva</trd>
  <trd lng="nl">wees</trd>
  <trd lng="ru">&c_s;&c_i;&c_r;&c_o;&c_t;&c_a;</trd>
  <trd lng="sv">f&ouml;r&auml;ldral&ouml;s</trd>
</drv>

<drv mrk="orf.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Rifu&gcirc;a establejo, kie oni zorgas ge<tld/>ojn.
    </dif>
  </snc>
  <trd lng="de">Waisenhaus</trd>
  <trd lng="hu">&aacute;rvah&aacute;z</trd>
  <trd lng="nl">weeshuis</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_t;&c_s;&c_k;&c_i;&c_j; &c_d;&c_o;&c_m;</trd>,
    <trd>&c_s;&c_i;&c_r;&c_o;&c_t;&c_s;&c_k;&c_i;&c_j;
&c_p;&c_r;&c_i;&c_ju;&c_t;</trd>
  </trdgrp>
  <trd lng="sv">barnhem</trd>
</drv>
</art>
