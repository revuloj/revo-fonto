<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>vort</rad>/o <fnt><bib>PV</bib></fnt>
    </kap>

    <drv mrk="vort.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="vort.0o.LIN">
        <tezrad fak="LIN"/>
        <tezrad fak="GRA"/>
        <uzo tip="fak">LIN</uzo>
        <dif>
          Areto da parolsonoj havanta difinitan signifon
          kaj difinitan gramatikan uzon en homa
          <ref tip="malprt" cel="lingv.0o.homa">lingvo</ref>:
          <ekz>
            simpla, kunmetita, dubsenca <tld/>o;
          </ekz><ekz>
            uzi la &gcirc;ustan, konvenan <tld/>on;
          </ekz><ekz>
            la bildoj elvokitaj de la <tld/>oj;
          </ekz><ekz>
            <tld/>ordo.
          </ekz>
        </dif>
        <subsnc mrk="vort.0o.esprimo">
          <dif>
            rigardata kiel formo, kontraste al la ideo:
            <ekz>
              oni ne devas diskuti pri <tld/>oj, sed peni ilin kompreni;
            </ekz><ekz>
              faroj estas fruktoj, sed <tld/>oj estas nur folioj;
            </ekz><ekz>
              kion vi legas mia princo? <tld/>ojn, <tld/>ojn, <tld/>ojn
              <fnt>Z</fnt>!
            </ekz>
          </dif>
        </subsnc>
        <subsnc mrk="vort.0o.diro">
          <dif>
            rigardata kiel eldiro unu- a&ubreve; plurvorta:
            <ekz>
              pli bone estas <tld/>o afabla ol kuko agrabla<fnt>Z</fnt>;
            </ekz><ekz>
              de <tld/>o &gcirc;entila ne doloras la lango
              <fnt><bib>PrV</bib></fnt>;
            </ekz><ekz>
              atentu miajn <tld/>ojn; Klinu vian orelon al miaj paroloj
              <fnt><bib>MT</bib>, <lok>&Sen; 4:20</lok></fnt>;
            </ekz><ekz>
              <tld/>on malveran virtulo malamas
              <fnt><bib>MT</bib>, <lok>&Sen; 13:5</lok></fnt>;
            </ekz><ekz>
              kiu &scirc;paras siajn <tld/>ojn, tiu estas prudenta
              <fnt><bib>MT</bib>, <lok>&Sen; 17:27</lok></fnt>;
            </ekz><ekz>
              per unu <tld/>o<fnt>Z</fnt>,
            </ekz><ekz>
              per malmultaj <tld/>oj
              <klr>(resume)</klr>
              <fnt>Z</fnt>;
            </ekz><ekz>
              diri la lim<tld/>on
              <klr>(ne cedi en disputo)</klr>
              <fnt>Z</fnt>;
            </ekz><ekz>
              lasi al iu la lastan <tld/>on
              <klr>(la finan decidon)</klr>
              <fnt>Z</fnt>;
            </ekz><ekz>
              ne venis ankora&ubreve; la tempo diri la lastan
              <tld/>on pri tiuj dubaj formoj
              <fnt>Z</fnt>;
            </ekz><ekz>
              sa&gcirc;a kapo duon<tld/>on
              <klr>(duone esprimitan penson)</klr> komprenas
              <fnt><bib>PrV</bib></fnt>.
            </ekz>
          </dif>
          <refgrp tip="vid">
            <ref cel="dir.0o">diro</ref>,
            <ref cel="parol.0o.fluo">parolo <sncref/></ref>
          </refgrp>
        </subsnc>
        <refgrp tip="vid">
          <ref cel="parol.0o.vorto">parolo <sncref/></ref>,
          <ref cel="termin.0o">termino</ref>
        </refgrp>
        <trdgrp lng="pl">
          <trd>s&lstroke;owo</trd>,
          <trd>wyraz</trd>
        </trdgrp>
      </snc>
      <snc mrk="vort.0o.promeso">
        <dif>
          Bu&scirc;a promeso:
          <ekz>
            teni, plenumi sian <tld/>on<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>o donita estas kiel le&gcirc;o
            <fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            estu la sinjoro de via <tld/>o<fnt>Z</fnt>;
          </ekz><ekz>
            je mia <tld/>o<fnt>Z</fnt><klr>(mi certigas pri tio)</klr>.
          </ekz>
        </dif>
        <trdgrp lng="fr">
          <trd>parole<klr> (promesse)</klr></trd>
        </trdgrp>
        <trd lng="pl">s&lstroke;owo</trd>
      </snc>
      <snc mrk="vort.0o.KOMP">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Negranda grupo da kune traktataj bitoj a&ubreve; signoj:
          <ekz>
            tiu komputilo operacias per 32-bitaj <tld/>oj;
          </ekz><ekz>
            por tiu redaktila komando &ccirc;iu interspaceta signogrupo
            estas <tld/>o.
          </ekz>
        </dif>
        <trd lng="pl">s&lstroke;owo</trd>
      </snc>
      <trd lng="be">&c_s;&c_l;&c_o;&c_v;&c_a;</trd>
      <trd lng="bg">&c_d;&c_u;&c_m;&c_a;</trd>
      <trd lng="br">ger</trd>
      <trd lng="cs">slovo</trd>
      <trd lng="de">Wort</trd>
      <trdgrp lng="el">
        <trd>&lambda;&epsilon_ton;&xi;&eta;</trd>,
        <trd>&lambda;&omikron_ton;&gamma;&omikron;&sigma_fina;</trd>,
        <trd>&omikron;&my;&jota;&lambda;&jota_ton;&alfa;</trd>
      </trdgrp>
      <trd lng="en">word</trd>
      <trd lng="es">palabra</trd>
      <trd lng="fr">mot</trd>
      <trd lng="he">&mem;&lamed;&he;</trd>
      <trd lng="hu">sz&oacute;</trd>
      <trd lng="kk">&c_s;&#1257;&c_z;</trd>
      <trd lng="nl">woord</trd>
      <trd lng="pt">palavra</trd>
      <trd lng="ru">&c_s;&c_l;&c_o;&c_v;&c_o;</trd>
      <trd lng="tr">s&ouml;z</trd>
      <trdgrp lng="zh">
        <trd>&#35789;</trd>,
        <trd><klr>tr. </klr>&#35422;<klr> [c&iacute;]</klr></trd>
      </trdgrp>
    </drv>

    <drv mrk="vort.V0o">
      <kap><tld lit="V"/>o</kap>
      <snc mrk="vort.V0o.KRI">
        <uzo tip="fak">KRI</uzo>
        <dif>
          Eterna sa&gcirc;o de Dio, enkarni&gcirc;inta en
          <ref tip="sin" cel="jesu.0o.vorto">Jesuo</ref>:
          <ekz>
            en la komenco estis la <tld lit="V"/>o,
            kaj la <tld lit="V"/>o estis kun Dio, kaj la <tld lit="V"/>o
            estis Dio
            <fnt><bib>NT</bib>, <lok>&Joh; 1:1</lok></fnt>;
          </ekz><ekz>
            la <tld lit="V"/>o fari&gcirc;is karno
            <fnt><bib>NT</bib>, <lok>&Joh; 1:14</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="br">Verb<klr tip="ind"> (kristeniezh)</klr></trd>
      <trd lng="de">Wort<klr tip="ind"> (biblisch)</klr></trd>
      <trdgrp lng="fr">
        <trd>verbe<klr> (christianisme)</klr></trd>
      </trdgrp>
      <trd lng="hu">ige<klr> (Krisztus)</klr></trd>
      <trd lng="lat">verbum</trd>
      <trd lng="pl">S&lstroke;owo</trd>
      <trd lng="ru">&c_S;&c_l;&c_o;&c_v;&c_o;</trd>
    </drv>

    <drv mrk="vort.0a">
      <kap><tld/>a</kap>
      <snc mrk="vort.0a.propre">
        <dif>
          Konsistanta el <tld/>oj:
          <ekz>
            <tld/>a ludo
            <klr>(sprita&jcirc;o)</klr>.
          </ekz>
        </dif>
        <trdgrp lng="pl">
          <trd>wyrazowy</trd>,
          <trd>s&lstroke;owny</trd>
        </trdgrp>
        <trd lng="ru">&c_s;&c_l;&c_o;&c_v;&c_je;&c_s;&c_n;&c_y;&c_j;</trd>
      </snc>
      <snc mrk="vort.0a.busxa">
        <dif>
          <ref tip="sin" cel="busx.0a.parola">Bu&scirc;a</ref>,
          konsistanta nur el <tld/>oj; neefektiva:
          <ekz>
            senvaloraj <tld/>aj promesoj<fnt>Z</fnt>
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="sxajn.0a">&scirc;ajna</ref>,
          <ref cel="paper">papera</ref>.
        </refgrp>
        <trd lng="br">dre gomz</trd>
        <trdgrp lng="fr">
          <trd>oral</trd>
        </trdgrp>
        <trdgrp lng="pl">
          <trd>s&lstroke;owny</trd>,
          <trd>ustny</trd>
        </trdgrp>
        <trd lng="ru">&c_u;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
      </snc>
      <trdgrp lng="be">
        <trd>&c_s;&c_l;&c_o;&c_w;&c_n;&c_y;</trd>,
        <trd>&c_s;&c_l;&c_a;&c_v;&c_je;&c_s;&c_n;&c_y;</trd>
      </trdgrp>
      <trd lng="hu">sz&oacute;-</trd>
    </drv>

    <drv mrk="vort.0aro">
      <kap><ofc>*</ofc><tld/>aro</kap>
      <snc mrk="vort.0aro.LIN">
        <uzo tip="fak">LIN</uzo>
        <dif>
          Orda <ref tip="super" cel="kolekt.0o.libr">kolekto</ref> da
          <tld/>oj de lingvo a&ubreve; fako kun difinoj, tradukoj a&ubreve;
          aliaj lingvaj informoj:
          <ekz>
            la <ctl>Fundamento de Esperanto</ctl> tute ne devas esti
            rigardata kiel la plej bona lernolibro kaj <tld/>aro de
            Esperanto
            <fnt><bib>F</bib>, <lok>anta&ubreve;parolo</lok></fnt>.
          </ekz>
        </dif>
        <refgrp tip="sub">
          <ref cel="leksik1.0o">leksikono</ref>
        </refgrp>
      </snc>
      <trd lng="be">&c_s;&c_l;&c_o;&c_w;&c_n;&c_ib;&c_k;</trd>
      <trd lng="bg">&c_r;&c_je;&c_ch;&c_n;&c_i;&c_k;</trd>
      <trd lng="br">geriadur</trd>
      <trd lng="de">W&ouml;rterbuch</trd>
      <trd lng="el">&lambda;&epsilon;&xi;&jota;&kappa;&omikron_ton;</trd>
      <trd lng="en">dictionary</trd>
      <trd lng="es">diccionario</trd>
      <trdgrp lng="fr">
        <trd>dictionnaire</trd>
      </trdgrp>
      <trd lng="he">&mem;&yod;&lamed;&vav;&fnun;</trd>
      <trd lng="hu">sz&oacute;t&aacute;r</trd>
      <trd lng="kk">&c_s;&#1257;&c_z;&c_d;&c_ib;&c_k;</trd>
      <trd lng="nl">woordenboek</trd>
      <trd lng="pl">s&lstroke;ownik</trd>
      <trd lng="pt">dicion&aacute;rio</trd>
      <trd lng="ru">&c_s;&c_l;&c_o;&c_v;&c_a;&c_r;&c_mol;</trd>
      <trdgrp lng="tr">
        <trd>s&ouml;zl&uuml;k</trd>,
        <trd>lug&acirc;t</trd>,
        <trd>l&uuml;gat</trd>
      </trdgrp>
      <trd lng="sk">slovn&iacute;k</trd>
      <trdgrp lng="zh">
        <trd>&#35789;&#20856;</trd>,
        <trd><klr>tr. </klr>&#35422;&#20856;<klr>
            [c&iacute;di&#462;n]</klr></trd>
      </trdgrp>
    </drv>

    <drv mrk="vort.0aristo">
      <kap><tld/>aristo</kap>
      <snc mrk="vort.0aristo.LIN">
        <dif>
          <ref tip="super" cel="lingv.0isto">Lingvisto</ref> studanta
          sencon
          kaj difinon de la <tld/>oj, kompilanta <tld/>arojn:
          <ekz>
            oni ne povas ju&gcirc;i lingvon la&ubreve; la <tld/>aristoj
            <fnt><bib>BonaLingvo</bib>, <lok>p. 81a</lok></fnt>
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_l;&c_je;&c_k;&c_s;&c_ib;&c_k;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
      <trd lng="br">geriadurour</trd>
      <trdgrp lng="fr">
        <trd>lexicographe</trd>
      </trdgrp>
      <trd lng="he">&mem;&yod;&lamed;&vav;&nun;&alef;&yod;</trd>
      <trdgrp lng="hu">
        <trd>sz&oacute;t&aacute;rk&eacute;sz&iacute;t&odblac;</trd>,
        <trd>lexikogr&aacute;fus</trd>
      </trdgrp>
      <trdgrp lng="pt">
        <trd>dicionarista</trd>,
        <trd>lexic&oacute;logo</trd>,
        <trd>lexic&oacute;grafo</trd>
      </trdgrp>
      <trd lng="ru">&c_l;&c_je;&c_k;&c_s;&c_i;&c_k;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
    </drv>

    <drv mrk="vort.0ero">
      <kap><tld/>ero</kap>
      <snc mrk="vort.0ero.GRA">
        <uzo tip="fak">GRA</uzo>
        <dif>
          Parto de <tld/>o, precipe senchava parto:
          <ekz>
            por montri la konsistigajn <tld/>erojn, en la skribo oni
            povas
            kunigi ilin per streketo: sport-klubo
            <fnt>
              <aut>B. Kolker</aut>,
              <lok><url
                  ref="http://www.xs4all.be/~esperan/lrg/LRG-1.html">
                  Lingva Ret-Gazeto - LRG-1</url>, [1996?]</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="fr">morph&egrave;me</trd>
    </drv>

    <drv mrk="vort.0igi">
      <kap><tld/>igi, <var><kap>en<tld/>igi</kap></var></kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="vort.0igi.KOMUNE">
        <dif>
          Esprimi per <tld/>oj:
          <ekz>
            kiam Johano en iu vespero sidis hejme kune kun Eriko, li
            komencis <tld/>igi siajn sentojn al la frato
            <fnt>
              <bib>InfanTorent2</bib>,
              <lok>dua parto, &ccirc;apitro 3a</lok>
            </fnt>;
          </ekz><ekz>
            ne<tld/>igita kondi&ccirc;o;
          </ekz><ekz>
            tamen &scirc;i ne en<tld/>igis siajn pensojn
            <fnt><aut>J. Baghy</aut>, <vrk>Sur Sanga Tero</vrk>,
              <lok>1933</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_v;&c_y;&c_r;&c_a;&c_zh;&c_a;&c_c;&c_mol; <ind>&c_s;&c_l;&c_o;&c_v;&c_a;&c_m;&c_ib;</ind></trd>,
        <trd>&c_f;&c_a;&c_r;&c_m;&c_u;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
      </trdgrp>
      <trd lng="br">geria&ntilde;</trd>
      <trdgrp lng="de">
        <trd>formulieren</trd>,
      </trdgrp>
      <trdgrp lng="fr">
        <trd>exprimer</trd>,
        <trd>formuler</trd>
      </trdgrp>
      <trd lng="he">&lamed;&nun;&samekh;&het;</trd>
      <trdgrp lng="hu">
        <trd>szavakba foglal</trd>,
        <trd>megfogalmaz</trd>
      </trdgrp>
      <trd lng="nl">onder <ind>woorden</ind> brengen</trd>
      <trdgrp lng="pl">
        <trd>wyra&zdot;a&cacute;<klr> s&lstroke;owami</klr></trd>,
        <trd>wys&lstroke;awia&cacute; si&eogonek;</trd>,
        <trd>artyku&lstroke;owa&cacute;</trd>,
        <trd>formu&lstroke;owa&cacute;</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_v;&c_y;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol; &c_s;&c_l;&c_o;&c_v;&c_a;&c_m;&c_i;</trd>,
        <trd>&c_f;&c_o;&c_r;&c_m;&c_u;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_s;&c_f;&c_o;&c_r;&c_m;&c_u;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
    </drv>

    <drv mrk="vort.a0o">
      <kap>a-<tld/>o</kap>
      <snc mrk="vort.a0o.GRA">
        <uzo tip="fak">GRA</uzo>
        <ref tip="dif" cel="adjekt.0o">adjektivo</ref>
        <ekz>
          jen kion signifas la a-<tld/>o <em>ortodoksa</em>: Konforma al
          la
          tradicia doktrino
          <fnt>
            <aut>Martin</aut>,
            <vrk>la bezono por lingvo universala</vrk>,
            <lok>soc.culture.esperanto, 2003-05-29</lok>
          </fnt>.
        </ekz>
      </snc>
      <trd lng="be">&c_p;&c_r;&c_y;&c_m;&c_je;&c_t;&c_n;&c_ib;&c_k;</trd>
      <trd lng="br">anv-gwan</trd>
      <trdgrp lng="de">
        <trd>Adjektiv</trd>,
        <trd>Eigenschaftswort</trd>
      </trdgrp>
      <trd lng="en">adjective</trd>
      <trd lng="es">adjetivo</trd>
      <trd lng="fr">adjectif</trd>
      <trd lng="hu">mell&eacute;kn&eacute;v</trd>
      <trd lng="it">aggettivo</trd>
      <trdgrp lng="nl">
        <trd>adjectief</trd>,
        <trd>bijvoeglijk <ind>naamwoord</ind></trd>
      </trdgrp>
      <trd lng="pl">przymiotnik</trd>
      <trd lng="pt">adjetivo</trd>
      <trd lng="ru">&c_p;&c_r;&c_i;&c_l;&c_a;&c_g;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je;</trd>
      <trd lng="sk">pr&iacute;davn&eacute; meno</trd>
      <trd lng="sv">adjektiv</trd>
    </drv>

    <drv mrk="vort.ali0e">
      <kap>ali<tld/>e</kap>
      <snc mrk="vort.ali0e.KOMUNE">
        <dif>
          Redirante la samon per aliaj <tld/>oj, a&ubreve; el alia vidpunkto:
          <ekz>
            tion postulas lia anta&ubreve;e alprenita sinteno, ali<tld/>e
            anta&ubreve;ju&gcirc;o
            <fnt><bib>BonaLingvo</bib>, <lok>p. 82a</lok></fnt>.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="nom.0e.te">nome <sncref/></ref>,
          <ref cel="est.tio0as">tio estas</ref>
        </refgrp>
      </snc>
      <trdgrp lng="br">
        <trd>eleze</trd>,
        <trd>da <ind>lavaret</ind> eo</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>in anderen Worten</trd>,
        <trd>mit anderen Worten</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>autrement dit</trd>,
        <trd>c'<ind>est-&agrave;-dire</ind></trd>,
        <trd>en d'autres <ind>termes</ind></trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>innymi s&lstroke;owy</trd>,
        <trd>inaczej m&oacute;wi&aogonek;c</trd>,
        <trd>inaczej</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_d;&c_r;&c_u;&c_g;&c_i;&c_m;&c_i; &c_s;&c_l;&c_o;&c_v;&c_a;&c_m;&c_i;</trd>,
        <trd>&c_i;&c_n;&c_a;&c_ch;&c_je; &c_g;&c_o;&c_v;&c_o;&c_r;&c_ja;</trd>,
        <trd>&c_t;&c_o; &c_je;&c_s;&c_t;&c_mol;</trd>
      </trdgrp>
    </drv>

    <drv mrk="vort.ar0o">
      <kap>ar<tld/>o</kap>
      <snc mrk="vort.ar0o.GRA">
        <uzo tip="fak">GRA</uzo>
        <dif>
          <ref tip="super" cel="substa1.0o">Substantivo</ref> tia, ke
          &gcirc;ia formo ununombra indikas aron da personoj a&ubreve; da
          similaj objektoj samspecaj, prezentatan kiel unu tuto:
          <ekz>
            <ctl>popolo</ctl>, <ctl>foliaro</ctl>, <ctl>herbo</ctl>,
            <ctl>armeo</ctl> estas ar<tld/>oj.
          </ekz>
        </dif>
      </snc>
      <trd lng="en">mass noun</trd>
      <trd lng="fr">collectif<klr> (grammaire)</klr></trd>
      <trd lng="ru">&c_s;&c_o;&c_b;&c_i;&c_r;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je; &c_i;&c_m;&c_ja;
        &c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je;</trd>
    </drv>

    <drv mrk="vort.e0o">
      <kap>e-<tld/>o</kap>
      <snc mrk="vort.e0o.GRA">
        <uzo tip="fak">GRA</uzo>
        <ref tip="dif" cel="adverb.0o">adverbo</ref>
        <ekz>
          e-<tld/>o povas roli kiel komplemento de verbo
          <fnt><bib>PMEG</bib>, <lok>Gramatiko, E-vortoj</lok></fnt>.
        </ekz>
      </snc>
      <trd lng="be">&c_p;&c_r;&c_y;&c_s;&c_l;&c_o;&c_w;&c_je;</trd>
      <trd lng="br">adverb</trd>
      <trdgrp lng="de">
        <trd>Adverb</trd>,
        <trd>Umstandswort</trd>
      </trdgrp>
      <trd lng="en">adverb</trd>
      <trd lng="es">adverbio</trd>
      <trd lng="fr">adverbe</trd>
      <trd lng="hu">hat&aacute;roz&oacute;sz&oacute;</trd>
      <trd lng="it">avverbio</trd>
      <trd lng="nl">bijwoord</trd>
      <trd lng="pl">przys&lstroke;&oacute;wek</trd>
      <trd lng="pt">adv&eacute;rbio</trd>
      <trd lng="ru">&c_n;&c_a;&c_r;&c_je;&c_ch;&c_i;&c_je;</trd>
      <trd lng="sk">pr&iacute;slovka</trd>
      <trd lng="sv">adverb</trd>
    </drv>

    <drv mrk="vort.evit0o">
      <kap>evit<tld/>o</kap>
      <snc mrk="vort.evit0o.LIN">
        <ref tip="dif" cel="euxfem.0o">e&ubreve;femismo</ref>
      </snc>
      <trd lng="be">&c_e;&c_w;&c_f;&c_e;&c_m;&c_ib;&c_z;&c_m;</trd>
      <trd lng="br">flourlavar</trd>
      <trdgrp lng="de">
        <trd>Euphemismus</trd>,
        <trd>Sch&ouml;nsprech</trd>,
        <trd>besch&ouml;nigender Ausdruck</trd>,
        <trd>verh&uuml;llender Ausdruck</trd>
      </trdgrp>
      <trd lng="es">eufemismo</trd>
      <trd lng="fr">euph&eacute;misme</trd>
      <trdgrp lng="hu">
        <trd>eufemizmus</trd>,
        <trd>euf&eacute;mia</trd>,
        <trd>sz&eacute;p&iacute;t&odblac; k&ouml;r&uuml;l&iacute;r&aacute;s
        </trd>
      </trdgrp>
      <trd lng="nl">eufemisme</trd>
      <trdgrp lng="pl">
        <trd>eufemizm</trd>
      </trdgrp>
      <trd lng="ru">&c_e;&c_v;&c_f;&c_je;&c_m;&c_i;&c_z;&c_m;</trd>
    </drv>

    <drv mrk="vort.fak0o">
      <kap>fak<tld/>o</kap>
      <snc mrk="vort.fak0o.LIN">
        <uzo tip="fak">LIN</uzo>
        <dif>
          Vorto a&ubreve; esprimo uzata kun difinita senco en iu scienco,
          arto,  metio, <ref tip="vid"
            cel="fak.0o.brancxo">fako</ref>...:
          <ekz>
            <ctl>Fak<tld/>oj en Esperanto</ctl>
            <fnt>
              <aut>W. Blanke</aut>,
              <vrk>Fakvortoj en Esperanto</vrk>,
              <lok>Deutsches E-lnstitut, 1995</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="fr">terme<klr> (terminol.)</klr></trd>
      <trd lng="pl">termin</trd>
    </drv>

    <drv mrk="vort.kap0o">
      <kap>kap<tld/>o</kap>
      <snc mrk="vort.kap0o.LIN">
        <uzo tip="fak">LIN</uzo>
        <dif>
          Vorto, kiun priskribas a&ubreve; komentas <ref tip="vid"
            cel="artiko.0o.sekcio">artikolo</ref> de vortaro a&ubreve;
          enciklopedio:
          <ekz>
            la kap<tld/>oj estas ordigitaj sen konsidero al la fina&jcirc;oj
            <fnt>
              <aut>Gaston Waringhien</aut>,
              <bib>PIV2</bib>,
              <lok>&leftquot;kiel uzi la vortaron&rightquot;, p. 34a</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="es">entrada<klr tip="amb"> (de
          diccionario...)</klr></trd>
      <trd lng="fr">entr&eacute;e<klr tip="amb"> (de dictionnaire...)</klr>
      </trd>
      <trdgrp lng="pl">
        <trd>has&lstroke;o</trd>
      </trdgrp>
    </drv>

    <drv mrk="vort.laux0e">
      <kap>la&ubreve;<tld/>e</kap>
      <snc mrk="vort.laux0e.KOMUNE">
        <dif>
          Konforme al anta&ubreve;e diritaj vortoj, sen modifo, kun nur
          minimuma interpreto:
          <ekz>
            la&ubreve;<tld/>e tradukite
            <fnt><bib>SatirRak</bib>, <lok>budape&scirc;ta ekzameno</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="br">ger-ha-ger</trd>
      <trdgrp lng="fr">
        <trd>&agrave; la lettre</trd>,
        <trd>mot pour mot</trd>,
        <trd>litt&eacute;ralement</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>dos&lstroke;ownie</trd>,
        <trd>s&lstroke;owo w s&lstroke;owo</trd>
      </trdgrp>
      <trd lng="ru">&c_d;&c_o;&c_s;&c_l;&c_o;&c_v;&c_n;&c_o;</trd>
    </drv>

    <drv mrk="vort.o0o">
      <kap>o-<tld/>o</kap>
      <snc mrk="vort.o0o.GRA">
        <uzo tip="fak">GRA</uzo>
        <ref tip="dif" cel="substa1.0o">substantivo</ref>
        <ekz>
          infinitivo rolas en Esperanto kiel o-<tld/>o
          <fnt>
            <aut>S. Hartwig</aut>,
            <vrk>Re: videblas</vrk>,
            <lok>soc.culture.esperanto, 2005-03-11</lok></fnt>.
        </ekz>
      </snc>
      <trd lng="be">&c_n;&c_a;&c_z;&c_o;&c_w;&c_n;&c_ib;&c_k;</trd>
      <trd lng="br">anv-kadarn</trd>
      <trdgrp lng="de">
        <trd>Substantiv</trd>,
        <trd>Nomen</trd>,
        <trd>Hauptwort</trd>
      </trdgrp>
      <trd lng="en">noun</trd>
      <trdgrp lng="es">
        <trd>sustantivo</trd>,
        <trd>nombre</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>substantif</trd>,
        <trd>nom<klr tip="amb"> (substantif)</klr></trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>f&odblac;n&eacute;v</trd>,
        <trd>substantivum</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>substantief</trd>,
        <trd>zelfstandig <ind>naamwoord</ind></trd>
      </trdgrp>
      <trd lng="pl">rzeczownik</trd>
      <trd lng="ru">&c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je;</trd>
    </drv>

    <drv mrk="vort.pas0o">
      <kap>pas<tld/>o</kap>
      <snc mrk="vort.pas0o.KOMUNE">
        <dif>
          Sekreta <tld/>o interkonsentita ene de limigita grupo kaj
          servanta kiel rekonilo, por pruvi onian rajton trapasi, eniri,
          ricevi servon ktp.
        </dif>
      </snc>
      <trd lng="be">&c_p;&c_a;&c_r;&c_o;&c_l;&c_mol;</trd>
      <trd lng="br">ger tremen</trd>
      <trd lng="de">Passwort</trd>
      <trd lng="en">password</trd>
      <trd lng="es">contrase&ntilde;a</trd>
      <trdgrp lng="fr">
        <trd>mot de passe</trd>
      </trdgrp>
      <trd lng="he">&samekh;&yod;&samekh;&mem;&he;</trd>
      <trd lng="hu">jelsz&oacute;</trd>
      <trd lng="nl">wachtwoord</trd>
      <trd lng="pl">has&lstroke;o</trd>
      <trd lng="pt">senha</trd>
      <trd lng="ru">&c_p;&c_a;&c_r;&c_o;&c_l;&c_mol;</trd>
    </drv>

    <drv mrk="vort.prunt0o">
      <kap>prunt<tld/>o</kap>
      <snc mrk="vort.prunt0o.LIN">
        <uzo tip="fak">LIN</uzo>
        <dif>
          <ref tip="super" cel="vort.0o.LIN">Vorto</ref> prenita el alia
          lingvo, eventuale skribe a&ubreve; elparole adaptita:
          <ekz>
            parolado de Filipinanoj havas multajn prunt<tld/>ojn el la
            Hispana kaj la Angla
            <fnt>
              <aut>V. Medrano</aut>,
              <vrk><url
                  ref="http://ca.geocities.com/vixcafe/esperanto/fil.html">Lingvoj
                  en Filipinoj</url></vrk>,
              <lok>2005-11-08</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="fr">
        <trd>emprunt linguistique</trd>,
        <trd>mot emprunt&eacute;</trd>
      </trdgrp>
      <trd lng="pl">zapo&zdot;yczenie<klr> j&eogonek;zykowe</klr></trd>
    </drv>

    <drv mrk="vort.sen0e">
      <kap>sen<tld/>e</kap>
      <snc mrk="vort.sen0e.KOMUNE">
        <dif>
          Sen paroloj, silente, ne donante atendatan respondon:
          <ekz>
            li sen<tld/>e obeis
            <fnt><bib>FK</bib>, <lok>4, rakontoj, Arturo</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_m;&c_o;&c_w;&c_ch;&c_k;&c_ib;</trd>,
        <trd>&c_b;&c_ja;&c_z; <ind>&c_s;&c_l;&c_o;&c_w;</ind></trd>
      </trdgrp>
      <trd lng="br">dilavar<klr tip="ind"> (adv.)</klr></trd>
      <trd lng="de">wortlos</trd>
      <trd lng="fr">sans un mot</trd>
      <trdgrp lng="pl">
        <trd>bez <ind>s&lstroke;owa</ind></trd>,
        <trd>milcz&aogonek;c</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_m;&c_o;&c_l;&c_ch;&c_a;</trd>,
        <trd>&c_n;&c_je; &c_g;&c_o;&c_v;&c_o;&c_r;&c_ja; &c_n;&c_i; &c_s;&c_l;&c_o;&c_v;&c_a;</trd>
      </trdgrp>
    </drv>

    <drv mrk="vort.unu0e">
      <kap>unu<tld/>e</kap>
      <snc mrk="vort.unu0e.KOMUNE">
        <dif>
          Dirite per unu a&ubreve; malmultaj vortoj, koncize, resume:
          <ekz>
            la &ccirc;efa militestro havas nek patron, nek infanon,
            unu<tld/>e li havas neniun, nur sian armeon kaj la malamikon
            <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 22a</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_a;&c_d;&c_n;&c_y;&c_m; <ind>&c_s;&c_l;&c_o;&c_v;&c_a;&c_m;</ind>
      </trd>
      <trdgrp lng="br">
        <trd>berr-ha-berr</trd>,
        <trd>e<ind>berr gomzo&ugrave;</ind></trd>,
        <trd>en ur <ind>ger</ind></trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>bref<klr tip="amb"> (adv.)</klr></trd>,
        <trd>en un mot</trd>
      </trdgrp>
      <trd lng="pl">jednym <ind>s&lstroke;owem</ind></trd>
      <trd lng="ru">&c_o;&c_d;&c_n;&c_i;&c_m; &c_s;&c_l;&c_o;&c_v;&c_o;&c_m;</trd>
    </drv>

    <drv mrk="vort.0odivido">
      <kap><tld/>odivido <fnt><bib>KompLeks</bib></fnt></kap>
      <snc mrk="vort.0odivido.LIN">
        <uzo tip="fak">LIN</uzo>
        <dif>
          Dispartigo de <tld/>o inter la fino unu teksta linio kaj la
          komenco de la tujsekva linio, precipe en okazo kiam en la
          unua linio ne eblas loki la vican <tld/>on, kvankam tie ja
          restas iom da spaco; streketo post la unua parto fine de la
          unua linio indikas, ke la <tld/>o estas da&ubreve;rigota en la sekva
          linio:
          <ekz>
            nesufi&ccirc;e profesia enpa&gcirc;igo &gcirc;enas plurloke, anka&ubreve; per
            mallertaj <tld/>odividoj
            <fnt>
              <aut>H. Gorecka</aut>:
              <vrk><url
              ref="http://www.esperanto.org/Ondo/Recenzoj/R-napolo.htm"
                  >Napolo, ne nur la kanzono&#8230;</url></vrk>.
              <bib>LOdE</bib>, <lok>2008. N-ro 8&ndash;9 (166&ndash;167)</lok>
            </fnt>.
          </ekz>
        </dif>
        <trd lng="en">hyphenation</trd>
        <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_n;&c_o;&c_s;</trd>
      </snc>
    </drv>

    <drv mrk="vort.0farado">
      <kap><tld/>farado</kap>
      <snc mrk="vort.0farado.LIN">
        <uzo tip="fak">LIN</uzo>
        <dif>
          Konstruado de <tld/>oj per kunmeto de vorteroj
          afiksaj a&ubreve; radikaj &ndash;
          <refgrp tip="sub">
            <ref cel="deriv.0i.GRA">derivado</ref>,
            <ref cel="vort.0kunmeto">vortkunmeto</ref>,
            <ref cel="fleksi.0o.paradigmo">fleksio <sncref/></ref>
          </refgrp>
          &ndash; eventuale kun &scirc;an&gcirc;oj en la <tld/>elementoj
          la&ubreve; la reguloj de koncerna lingvo:
          <ekz>
            tiuj &ccirc;i naturaj <tld/>faraj oportuna&jcirc;oj, kiujn la
            lingvo Esperanto havas
            <fnt><bib>EE</bib></fnt>;
          </ekz><ekz>
            la principo de neceso kaj sufi&ccirc;o la&ubreve; Ren&eacute; de
            Saussure estas oficiale akceptita de la Akademio kiel
            <ctl>la logika bazo de la vortfarado en Esperanto</ctl>
            <fnt><bib>TermKurs</bib>, <lok>&FE; 4.915</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_s;&c_l;&c_o;&c_v;&c_a;&c_w;&c_t;&c_v;&c_a;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
      <trd lng="br">ar stumma&ntilde; <ind>gerio&ugrave;</ind></trd>
      <trd lng="de">Wordbildung</trd>
      <trd lng="en">word-formation</trd>
      <trdgrp lng="fr">
        <trd>construction morphologique<klr tip="amb">
            (linguistique)</klr></trd>
      </trdgrp>
      <trd lng="hu">sz&oacute;alkot&aacute;s</trd>
      <trd lng="nl">woordvorming</trd>
      <trd lng="pl">s&lstroke;owotw&oacute;rstwo</trd>
      <trdgrp lng="ru">
        <trd>&c_s;&c_l;&c_o;&c_v;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
        <trd>&c_s;&c_l;&c_o;&c_v;&c_o;&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_o;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>
      </trdgrp>
    </drv>

    <drv mrk="vort.0formo">
      <kap><tld/>formo</kap>
      <snc mrk="vort.0formo.LIN">
        <uzo tip="fak">LIN</uzo>
        <dif>
          <tld lit="V"/>o prenita en iu gramatika formo:
          <ekz>
            <ctl>tabloj</ctl> kaj <ctl>tablon</ctl> estas du
            <tld/>formoj de unu sama <tld/>o.
          </ekz>
        </dif>
        <ref tip="vid" cel="fleksi.0i">fleksii</ref>
      </snc>
      <trd lng="be">&c_s;&c_l;&c_o;&c_v;&c_a;&c_f;&c_o;&c_r;&c_m;&c_a;</trd>
      <trd lng="br">furm<klr tip="amb"> (ger)</klr></trd>
      <trdgrp lng="en">
        <trd>word form</trd>, <trd>inflexion</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>forme fl&eacute;chie</trd>
      </trdgrp>
      <trdgrp lng="he">
        <trd>&nun;&tet;&yod;&he; &dalet;&qof;&dalet;&vav;&qof;&yod;&tav;
        </trd>,
        <trd>&tsadi;&vav;&resh;&he;</trd>
      </trdgrp>
      <trd lng="hu">sz&oacute;alak</trd>
      <trd lng="nl">woordvorm</trd>
      <trd lng="ru">&c_s;&c_l;&c_o;&c_v;&c_o;&c_f;&c_o;&c_r;&c_m;&c_a;</trd>
    </drv>

    <drv mrk="vort.0kunmeto">
      <kap><tld/>kunmeto</kap>
      <snc mrk="vort.0kunmeto.LIN">
        <uzo tip="fak">LIN</uzo>
        <dif>
          Konstruado de <tld/>oj per kunmeto de
          <ref tip="vid" cel="radika1.0o.GRA">radikaloj</ref>:
          <ekz>
            post tiu transpreno, nek la metron de <ctl>Sinjoro
            Tadeo</ctl>,
            nek la <tld/>kunmetojn de adjektivo plus substantivo nia
            lingvanaro akceptas kiel normala&jcirc;ojn
            <fnt>
              <aut>P. Da&scirc;gupto</aut>,
              <vrk>Pracelojn fasonis recelano (Z. Banet-Fornalowa,
                Antoni Grabowski)</vrk>,
              <bib>Monato</bib>,
              <lok>jaro 2002a, numero 3a, p. 26a</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <ref tip="super" cel="vort.0farado"><tld/>farado</ref>
      <trd lng="br">kenstrollad<klr tip="ind"> (anvio&ugrave;,
          gerio&ugrave;)</klr></trd>
      <trdgrp lng="en">
        <trd>composition</trd>,
        <trd>compounding</trd>
      </trdgrp>
      <trd lng="fr">composition<klr> (de mots)</klr></trd>
      <trdgrp lng="ru">
        <trd>&c_s;&c_l;&c_o;&c_v;&c_o;&c_s;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>,
        <trd>&c_o;&c_s;&c_n;&c_o;&c_v;&c_o;&c_s;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
      </trdgrp>
    </drv>

    <drv mrk="vort.0oprovizo">
      <kap><tld/>oprovizo, <var><kap><tld/>provizo</kap></var></kap>
      <uzo tip="fak">LIN</uzo>
      <snc mrk="vort.0oprovizo.LIN">
        <dif>
          &Ccirc;iom da <tld/>oj de
          <ref tip="malprt" cel="lingv.0o.homa">lingvo<sncref/></ref>
          nacia, de regiona dialekto, de profesia fako, de lerneja kurso
          ktp:
          <ekz>
            dank' al ili <klr>(la afiksoj de Esperanto)</klr> koreoj
            tre facile konkeras la <tld/>oprovizon de Esperanto
            <fnt>
              <aut>Bak G.</aut>,
              <vrk>Esperanto sentata de koreoj</vrk>,
              <lok>EK, numero 17a</lok>
            </fnt>;
          </ekz><ekz>
            anta&ubreve; ilia efektivigo <klr>...</klr> la ellaboro de
            konsekvenca <tld/>oprovizo ne estas ebla
            <fnt>
              <aut>E. W&uuml;ster</aut>,
              <vrk>Esperantologiaj studoj</vrk>,
              <lok>Stafeto, 1978, p. 27a</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="br">geriaoueg</trd>
      <trd lng="de">Wortschatz</trd>
      <trd lng="en">vocabulary</trd>
      <trd lng="es">vocabulario</trd>
      <trd lng="fr">vocabulaire</trd>
      <trdgrp lng="hu">
        <trd>sz&oacute;k&eacute;szlet</trd>,
        <trd>sz&oacute;&aacute;llom&aacute;ny</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>s&lstroke;ownictwo</trd>,
        <trd>zas&oacute;b s&lstroke;&oacute;w</trd>
      </trdgrp>
      <trd lng="ru">&c_s;&c_l;&c_o;&c_v;&c_a;&c_r;&c_n;&c_y;&c_j; &c_z;&c_a;&c_p;&c_a;&c_s;</trd>
    </drv>
  </art>
