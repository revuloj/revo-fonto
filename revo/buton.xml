<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>buton</rad>/o
</kap>
<drv mrk="buton.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="buton.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Plata a&ubreve; globforma peceto el korno, metalo a&ubreve;
&scirc;tofo,
      uzata por kunteni partojn de vestoj a&ubreve; de aliaj objektoj, kaj
      kelkfoje nur por ornami:
      <ekz>
        <tld/>o de jako, de sako, de manumo, de &scirc;uo.
      </ekz>
    </dif>
    <trd lng="en">button</trd>
    <trd lng="de">Knopf</trd>
    <trd lng="hu">gomb</trd>
    <trd lng="nl">knoop</trd>
    <trd lng="ru">&c_p;&c_u;&c_g;&c_o;&c_v;&c_i;&c_c;&c_a;</trd>
    <trd lng="sk">gomb&iacute;k</trd>
    <trd lng="sv">knapp</trd>
  </snc>
  <snc mrk="buton.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Io similanta <tld/>on:
      <ekz>
        <uzo tip="fak">ELE</uzo>
        prem<tld/>o de elektra sonorilo;
      </ekz>
      <ekz>
        <uzo tip="fak">KOMP</uzo>
        maldekstra, meza, dekstra <ind>mus<tld/>o</ind>
        <klr>(prem<tld/>oj de
        <ref tip="malprt" cel="mus.0o.KOMP">muso</ref>)</klr>.
        <trd lng="en">mouse button</trd>
        <trd lng="ru">&c_k;&c_n;&c_o;&c_p;&c_k;&c_a;
&c_m;&c_y;&c_sh;&c_i;</trd>
      </ekz>
    </dif>
    <trd lng="en">button</trd>
    <trd lng="de">Knopf</trd>
    <trd lng="hu">gomb</trd>
    <trd lng="nl">knop</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_n;&c_o;&c_p;&c_k;&c_a; <klr>(&c_d;&c_l;&c_ja;
&c_n;&c_a;&c_zh;&c_i;&c_m;&c_a;&c_n;&c_i;&c_ja;)</klr></trd>,
      <trd>&c_g;&c_o;&c_l;&c_o;&c_v;&c_k;&c_a;
<klr>(&c_k;&c_a;&c_k;&c_o;&c_g;&c_o;-&c_l;.
&c_p;&c_r;&c_je;&c_d;&c_m;&c_je;&c_t;&c_a;)</klr></trd>
    </trdgrp>
    <trd lng="sk">tla&ccaron;idlo</trd>
    <trd lng="sv">knapp</trd>
  </snc>
  <snc mrk="buton.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Bur&gcirc;ono de floro; florkapeto.
    </dif>
    <trd lng="en">bud</trd>
    <trdgrp lng="hu">
      <trd>bimb&oacute;</trd>,
      <trd>r&uuml;gy</trd></trdgrp>
    <trd lng="lat">gemma, ae</trd>
    <trd lng="nl">knop</trd>
    <trd lng="ru">&c_b;&c_u;&c_t;&c_o;&c_n;</trd>
  </snc>
  <trd lng="es">bot&oacute;n</trd>
  <trd lng="fr">bouton</trd>
</drv>

<drv mrk="buton.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kunigi per <tld/>oj, enigante ilin en la destinitajn fendetojn:
      <ekz>
        <tld/>umi surtuton pro malvarmo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>umi iun malvaste
        <klr>(severe obeigi iun)</klr><fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="sin" cel="buton.0i">butoni</ref>
  <trd lng="de">zukn&ouml;pfen</trd>
  <trd lng="es">abotonar</trd>
  <trd lng="fr">boutonner</trd>
  <trd lng="hu">begombol</trd>
  <trd lng="nl">knopen</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_s;&c_t;&c_jo;&c_g;&c_i;&c_v;&c_a;&c_t;&c_mol;
<klr>(&c_n;&c_a; &c_p;&c_u;&c_g;&c_o;&c_v;&c_i;&c_c;&c_y;)</klr></trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_je;&c_g;&c_n;&c_u;&c_t;&c_mol;
<klr>(&c_n;&c_a; &c_p;&c_u;&c_g;&c_o;&c_v;&c_i;&c_c;&c_y;)</klr></trd>
  </trdgrp>
  <trd lng="sk">zapn&uacute;&tcaron; <klr>(gomb&iacute;ky)</klr></trd>
  <trd lng="sv">kn&auml;ppa</trd>
</drv>

<drv mrk="buton.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <ref tip="dif" cel="buton.0umi">butonumi</ref>
  <trd lng="de">zukn&ouml;pfen</trd>
  <trd lng="es">abotonar</trd>
  <trd lng="fr">boutonner</trd>
  <trd lng="hu">begombol</trd>
  <trd lng="nl">knopen</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_s;&c_t;&c_jo;&c_g;&c_i;&c_v;&c_a;&c_t;&c_mol;
<klr>(&c_n;&c_a; &c_p;&c_u;&c_g;&c_o;&c_v;&c_i;&c_c;&c_y;)</klr></trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_je;&c_g;&c_n;&c_u;&c_t;&c_mol;
<klr>(&c_n;&c_a; &c_p;&c_u;&c_g;&c_o;&c_v;&c_i;&c_c;&c_y;)</klr></trd>
  </trdgrp>
  <trd lng="sv">kn&auml;ppa</trd>
</drv>

<drv mrk="buton.mal0umi">
  <kap>mal<tld/>(um)i</kap>
  <snc>
    <dif>
      Disigi partojn de vesto eligante la <tld/>ojn el la fendoj.
    </dif>
  </snc>
  <trd lng="de">aufkn&ouml;pfen</trd>
  <trd lng="es">desabotonar</trd>
  <trd lng="fr">d&eacute;boutonner</trd>
  <trd lng="hu">kigombol</trd>
  <trd lng="nl">losknopen</trd>
  <trdgrp lng="ru">
 
<trd>&c_r;&c_a;&c_s;&c_s;&c_t;&c_jo;&c_g;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_s;&c_t;&c_je;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">rozopn&uacute;&tcaron;</trd>
  <trd lng="sv">kn&auml;ppa upp</trd>
</drv>

<drv mrk="buton.0truo">
  <kap><tld/>truo</kap>
  <snc>
    <dif>
      Fendeto por enigi la <tld/>on.
    </dif>
  </snc>
  <trd lng="de">Knopfloch</trd>
  <trd lng="fr">boutonni&egrave;re</trd>
  <trd lng="hu">gomblyuk</trd>
  <trd lng="nl">knoopsgat</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_t;&c_l;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_t;&c_l;&c_ja; <klr tip="amb">(&c_d;&c_l;&c_ja;
&c_p;&c_u;&c_g;&c_o;&c_v;&c_i;&c_c;&c_y;)</klr></trd>
  </trdgrp>
  <trd lng="sv">knapph&aring;l</trd>
</drv>

<drv mrk="buton.ekrana0o">
  <kap>ekrana <tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="fenest.0ajxo">Fenestra&jcirc;o</ref>, areo
      de komputila ekrano entenanta tekston kaj,
      kutime, desegna&jcirc;on de <tld/>o &#8212; ortogramon a&ubreve;
rondeton, iam
      bildigitan per la Askiaj rektaj a&ubreve; rondaj krampoj: [ ] ( ). La
      uzanto povas &scirc;an&gcirc;i la staton de la areo (kaj de la
responda
      variablo &#8212; re&gcirc;imo, opcio) per la muso a&ubreve; per la
klavaro:
      <ekz>
        oni distingas tri <tld/>specojn: prem<tld/>o, marko<tld/>o kaj
        radio<tld/>o;
      </ekz>
      <ekz>
        en <ref cel="dialog.0ujo">dialogujo</ref> kutime estas
        <tld/>oj &leftquot;Bone&rightquot;, &leftquot;Rezignu&rightquot;,
ofte anka&ubreve; &leftquot;Jes&rightquot; kaj &leftquot;Ne&rightquot;.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">screen <ind>button</ind></trd>
  <trd lng="hu">k&eacute;perny&odblac;gomb</trd>
  <trd lng="ru">&c_e;&c_k;&c_r;&c_a;&c_n;&c_n;&c_a;&c_ja;
<ind>&c_k;&c_n;&c_o;&c_p;&c_k;&c_a;</ind></trd>
</drv>
</art>
