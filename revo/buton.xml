<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>buton</rad>/o
</kap>
<drv mrk="buton.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Plata a&ubreve; globforma peceto el korno, metalo a&ubreve; &scirc;tofo,
      uzata por kunteni partojn de vestoj a&ubreve; de aliaj objektoj, k
      kelkfoje nur por ornami:
      <ekz>
        <tld/>o de jako, de sako, de manumo, de
        &scirc;uo.
      </ekz>
    </dif>
    <trd lng="de">Knopf</trd>
    <trd lng="nl">knoop</trd>
    <trd lng="ru">&c_p;&c_u;&c_g;&c_o;&c_v;&c_i;&c_c;&c_a;</trd>
  </snc>
  <snc num="2">
    <dif>
      Io similanta <tld/>on:
      <ekz>
        prem<tld/>o de elektra sonorilo.
      </ekz>
    </dif>
    <trd lng="de">Knopf</trd>
    <trd lng="nl">knop</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_n;&c_o;&c_p;&c_k;&c_a; <klr>(&c_d;&c_l;&c_ja; &c_n;&c_a;&c_zh;&c_i;&c_m;&c_a;&c_n;&c_i;&c_ja;)</klr></trd>,
      <trd>&c_g;&c_o;&c_l;&c_o;&c_v;&c_k;&c_a; <klr>(&c_k;&c_a;&c_k;&c_o;&c_g;&c_o;-&c_l;. &c_p;&c_r;&c_je;&c_d;&c_m;&c_je;&c_t;&c_a;)</klr></trd>
    </trdgrp>
  </snc>
  <snc num="3">
    <dif>
      Bur&gcirc;ono de floro; florkapeto. 
    </dif>
   <trd lng="nl">knop</trd>
   <trd lng="ru">&c_b;&c_u;&c_t;&c_o;&c_n;</trd>
  </snc>
  <trd lng="en">button</trd>
</drv>
<drv mrk="buton.0umi">
  <kap><tld/>(um)i</kap> 
  <gra><vspec>tr</vspec></gra> 
  <snc>
    <dif>
      Kunigi per <tld/>oj, enigante ilin en la destinitajn fendetojn: 
      <ekz>
        <tld/>umi surtuton pro malvarmo <klr>(f)</klr>;
      </ekz>
      <ekz>
        <tld/>umi iun malvaste<fnt>Z</fnt>
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>
        <klr>(severe obeigi iun)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">zukn&ouml;pfen</trd>
  <trd lng="nl">knopen</trd>
  <trd lng="ru">&c_z;&c_a;&c_s;&c_t;&#1105;&c_g;&c_i;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_n;&c_a; &c_p;&c_u;&c_g;&c_o;&c_v;&c_i;&c_c;&c_y;)</klr></trd>
</drv>
<drv mrk="buton.mal0umi">
  <kap>mal<tld/>(um)i</kap>
  <snc>
    <dif>
      Disigi partojn de vesto eligante la <tld/>ojn el la fendoj. 
    </dif>
  </snc>
  <trd lng="de">aufkn&ouml;pfen</trd>
  <trd lng="nl">losknopen</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_s;&c_t;&#1105;&c_g;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="buton.0truo">
  <kap><tld/>truo</kap>
  <snc>
    <dif>
      Fendeto por enigi la <tld/>on.  
    </dif>
  </snc>
  <trd lng="de">Knopfloch</trd>
  <trd lng="nl">knoopsgat</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_t;&c_l;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_t;&c_l;&c_ja; <klr>(&c_d;&c_l;&c_ja; &c_p;&c_u;&c_g;&c_o;&c_v;&c_i;&c_c;&c_y;)</klr></trd>
  </trdgrp>
</drv>
</art>
