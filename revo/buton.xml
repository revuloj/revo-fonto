<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>buton</rad>/o
</kap>
<drv mrk="buton.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Plata a&ubreve; globforma peceto el korno, metalo a&ubreve; &scirc;tofo,
      uzata por kunteni partojn de vestoj a&ubreve; de aliaj objektoj, k
      kelkfoje nur por ornami:
      <ekz>
        <tld/>o de jako, de sako, de manumo, de
        &scirc;uo.
      </ekz>
    </dif>
    <trd lng="de">Knopf</trd>
  </snc>
  <snc num="2">
    <dif>
      Io similanta <tld/>on:
      <ekz>
        prem<tld/>o de elektra sonorilo.
      </ekz>
    </dif>
    <trd lng="de">Knopf</trd>
  </snc>
  <snc num="3">
    <dif>
      Bur&gcirc;ono de floro; florkapeto. 
    </dif>
  </snc>
  <trd lng="en">button</trd>
</drv>
<drv mrk="buton.0umi">
  <kap><tld/>(um)i</kap> 
  <gra><vspec>tr</vspec></gra> 
  <snc>
    <dif>
      Kunigi per <tld/>oj, enigante ilin en la destinitajn fendetojn: 
      <ekz>
        <tld/>umi surtuton pro malvarmo <klr>(f)</klr>;
      </ekz>
      <ekz>
        <tld/>umi iun malvaste<fnt>Z</fnt>
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>
        <klr>(severe obeigi iun)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">zukn&ouml;pfen</trd>
</drv>
<drv mrk="buton.mal0umi">
  <kap>mal<tld/>(um)i</kap>
  <snc>
    <dif>
      Disigi partojn de vesto eligante la <tld/>ojn el la fendoj. 
    </dif>
  </snc>
  <trd lng="de">aufkn&ouml;pfen</trd>
</drv>
<drv mrk="buton.0truo">
  <kap><tld/>truo</kap>
  <snc>
    <dif>
      Fendeto por enigi la <tld/>on.  
    </dif>
  </snc>
  <trd lng="de">Knopfloch</trd>
</drv>
</art>
<!--
$Log$
-->
</vortaro>