<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>restoraci</rad>/o
</kap>
<drv mrk="restor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Establejo, kie pagante oni povas man&gcirc;i kaj trinki.
    </dif>
    <refgrp tip="sin">
      <ref cel="gast.0ejo">gastejo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_s;&c_t;&c_a;&c_r;&c_a;&c_n;</trd>
  <trdgrp lng="de">
    <trd>Restaurant</trd>,
    <trd>Gasthaus</trd>,
    <trd>Wirtshaus</trd>,
    <trd>Wirtschaft</trd>
  </trdgrp>
  <trd lng="en">restaurant</trd>
  <trd lng="es">restaurante</trd>
  <trd lng="fr">restaurant</trd>
  <trd lng="hu">&eacute;tterem</trd>  
  <trd lng="nl">restaurant</trd>
  <trd lng="pt">restaurante</trd>
  <trd lng="ru">&c_r;&c_je;&c_s;&c_t;&c_o;&c_r;&c_a;&c_n;</trd>
</drv>
</art>
