<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kaolin</rad>/o
</kap>
<drv mrk="kaolin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Blanka argilo, el kiu oni fabrikas porcelanon.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_a;&c_l;&c_ib;&c_n;</trd>
  <trdgrp lng="de">
    <trd>Porzellanerde</trd>,
    <trd>Kaolin</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>kaolin</trd>,
    <trd>terra alba</trd>
  </trdgrp>
  <trd lng="fr">kaolin</trd>
  <trdgrp lng="hu">
    <trd>kaolin</trd>,
    <trd>porcel&aacute;nf&ouml;ld</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kaolien</trd>,
    <trd>porseleinaarde</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_o;&c_l;&c_i;&c_n;</trd>,
    <trd>&c_b;&c_je;&c_l;&c_a;&c_ja; &c_g;&c_l;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="kaolin.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Estanta el <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_a;&c_l;&c_ib;&c_n;&c_a;&c_v;&c_y;</trd>
  <trdgrp lng="fr">
    <trd>kaolinique</trd>,
    <trd>de <ind>kaolin</ind></trd>
  </trdgrp>
  <trd lng="hu">kaolin-</trd>
</drv>

<drv mrk="kaolin.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      I&gcirc;i <tld/>o, transformi&gcirc;i en <tld/>on.
    </dif>
  </snc>
  <trd lng="fr">se <ind>kaoliniser</ind></trd>
</drv>
</art>
