<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>kaolin</rad>/o
</kap>
<drv mrk="kaolin.0o">
<kap><tld/>o</kap>
<uzo tip="fak">KEM</uzo>
<snc>
<dif>
Blanka argilo, el kiu oni fabrikas porcelanon. 
</dif>
</snc>
<trdgrp lng="en">
  <trd>kaolin</trd>,
  <trd>terra alba</trd>
</trdgrp>
</drv>
<drv mrk="kaolin.0a">
<kap><tld/>a</kap>
<snc>
<dif>
Estanta el <tld/>o. 
</dif>
</snc>
</drv>
<drv mrk="kaolin.0igxi">
<kap><tld/>i&gcirc;i</kap>
<snc>
<dif>
I&gcirc;i <tld/>o, transformi&gcirc;i en <tld/>on.  
</dif>
</snc>
</drv>
</art>
