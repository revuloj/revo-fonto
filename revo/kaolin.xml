<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kaolin</rad>/o
</kap>
<drv mrk="kaolin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Blanka argilo, el kiu oni fabrikas porcelanon. 
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>kaolin</trd>,
    <trd>terra alba</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kaolien</trd>,
    <trd>porseleinaarde</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_o;&c_l;&c_i;&c_n;</trd>,
    <trd>&c_b;&c_je;&c_l;&c_a;&c_ja; &c_g;&c_l;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
</drv>
<drv mrk="kaolin.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Estanta el <tld/>o. 
    </dif>
  </snc>
</drv>
<drv mrk="kaolin.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      I&gcirc;i <tld/>o, transformi&gcirc;i en <tld/>on.  
    </dif>
  </snc>
</drv>
</art>
