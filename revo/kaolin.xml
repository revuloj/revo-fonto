<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kaolin.xml,v 1.24 2023/12/03 08:00:22 revo Exp $">
<kap>
  <rad>kaolin</rad>/o
</kap>
<drv mrk="kaolin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Blanka argilo, el kiu oni fabrikas <ref cel="porcel.0o">porcelano</ref>:
      <ekz>
        en Burundo trovi&gcirc;as gravaj minejoj de feldspato, <tld/>o,
        fosforo, metaloj de la platina grupo <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=447">Burundo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        halojzito estas argilo kun kristala strukturo malsimila al tiu,
        de <tld/>o kaj kun pli grandaj blankeco kaj kemia pureco
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=119">Argilo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_a;&c_l;&c_ib;&c_n;</trd>
  <trdgrp lng="ca">
    <trd>caol&iacute;</trd>
  </trdgrp>
  <trd lng="cs">kaolin</trd>
  <trdgrp lng="de">
    <trd>Porzellanerde</trd>,
    <trd>Kaolin</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>kaolin</trd>,
    <trd>terra alba</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>caol&iacute;n</trd>
  </trdgrp>
  <trd lng="fr">kaolin</trd>
  <trdgrp lng="hu">
    <trd>kaolin</trd>,
    <trd>porcel&aacute;nf&ouml;ld</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x9AD8;&#x9675;&#x571F;  <pr>&#x3053;&#x3046;&#x308A;&#x3087;&#x3046;&#x3069;</pr></trd>,
    <trd>&#x767D;&#x9676;&#x571F;  <pr>&#x3057;&#x308D;&#x9676;&#x571F;</pr></trd>,
    <trd>&#x30AB;&#x30AA;&#x30EA;&#x30F3;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kaolien</trd>,
    <trd>porseleinaarde</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kaolin</trd>,
    <trd>glinka <ind>kaolinowa</ind></trd>
  </trdgrp>
  <trd lng="ro">caolin</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_o;&c_l;&c_i;&c_n;</trd>,
    <trd>&c_b;&c_je;&c_l;&c_a;&c_ja; &c_g;&c_l;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">kaol&iacute;n</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_a;&#x43E;&#x43B;&c_ib;&#x43D;</trd>,
    <trd>&c_b;&c_ib;&#x43B;&c_a; &c_g;&#x43B;&c_i;&#x43D;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9AD8;&#x5CAD;&#x571F; <pr>g&#x101;ol&#x1D0;ngt&#x1D4;</pr></trd>,
    <trd>&#x9676;&#x571F; <pr>t&#xE1;ot&#x1D4;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kaolin.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o; el <tld/>o:
      <ekz>
        <tld/>a argilo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=534324">Natria
          pirosulfato</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_a;&c_l;&c_ib;&c_n;&c_a;&c_v;&c_y;</trd>
  <trd lng="ca">caol&iacute;nic</trd>
  <trdgrp lng="fr">
    <trd>kaolinique</trd>,
    <trd>de <ind>kaolin</ind></trd>
  </trdgrp>
  <trd lng="hu">kaolin-</trd>
  <trdgrp lng="ja">
    <trd>&#x30AB;&#x30AA;&#x30EA;&#x30F3;&#x306E;</trd>,
    <trd>&#x30AB;&#x30AA;&#x30EA;&#x30F3;&#x88FD;&#x306E;  <pr>&#x30AB;&#x30AA;&#x30EA;&#x30F3;&#x305B;&#x3044;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">van <ind>kaolien</ind></trd>
  <trd lng="pl">kaolinowy</trd>
</drv>

<drv mrk="kaolin.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      I&gcirc;i <tld/>o, transformi&gcirc;i en <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_a;&c_l;&c_ib;&c_n;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">produir-se la caolinitzaci&oacute;</trd>
  <trd lng="fr">se <ind>kaoliniser</ind></trd>
  <trd lng="ja">&#x30AB;&#x30AA;&#x30EA;&#x30F3;&#x5316;&#x3059;&#x308B;  <pr>&#x30AB;&#x30AA;&#x30EA;&#x30F3;&#x304B;&#x3059;&#x308B;</pr></trd>
  <trd lng="nl">kaoliniseren</trd>
</drv>
</art>
