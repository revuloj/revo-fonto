<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: dism.xml,v 1.5 2019/10/03 14:10:19 revo Exp $">
<kap>
  <rad>dism</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="dism.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="dism.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="dif" cel="dek.0onajxo.EKON">Dekona&jcirc;o</ref>:
      <ekz>
        &ccirc;iuj religioj oficiale akiras televidajn kanalojn, unue danke al la <tld/>o de
        siaj &scirc;afoj kaj due danke al politikaj manipuladoj
        <fnt>
          <bib>Monato</bib>
          <aut>Gilbert Ledon</aut>
          <vrk><url ref="https://www.monato.be/2003/008040.html">Kvina povo</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        la koncernaj sumoj fari&gcirc;as <tld/>oj, favore al la 
        Civito
        <fnt>
          <aut>Esperanta Civito</aut>,
          <vrk><url ref="http://www.esperantio.net/index.php?id=437">
          Responde al oftaj demandoj</url></vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <!-- forman maloftan traduku per malofta&jcirc;o se tia ekzistas -->
  <trdgrp lng="de">
    <trd>Zehnter</trd>,
    <trd>Dezem</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>desiatok</trd>,
    <trd>de&zcaron;ma</trd>
  </trdgrp>
  <trd lng="fr">d&icirc;me</trd>
  <trd lng="pl">dziesi&eogonek;cina</trd>
</drv>
</art>

