<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kolofon</rad>/o
</kap>
<drv mrk="kolofo.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Speco de rezino, uzata por froti la haregojn de la ar&ccirc;oj,
      por ke ili skrapetu la kordojn.  
    </dif>
    <refgrp tip="hom">
      Ne konfuzu kun:
      <ref cel="kolofo1.0o">kolofono</ref> (presarto)
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_ib;&c_f;&c_o;&c_l;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>Harz</trd>,
    <trd>Kolophonium</trd>
  </trdgrp>
  <trd lng="en">rosin</trd>
  <trd lng="fr">colophane</trd>
  <trd lng="nl">hars</trd>
  <trd lng="ru">&c_k;&c_a;&c_n;&c_i;&c_f;&c_o;&c_l;&c_mol;</trd>
</drv>
</art>
