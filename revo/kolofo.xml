<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kolofon</rad>/o
</kap>
<drv mrk="kolofo.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Speco de 
      <ref tip="super" cel="rezin.0o">rezino</ref>, 
      uzata por froti la haregojn de la ar&ccirc;oj,
      por ke ili skrapetu la kordojn.  
    </dif>
    <refgrp tip="hom">
      Ne konfuzu kun:
      <ref cel="kolofo1.0o">kolofono</ref> (presarto)
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_ib;&c_f;&c_o;&c_l;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>colof&ograve;nia</trd>,
    <trd>pega grega</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>explicit</trd>,
    <trd>kolofon</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Harz</trd>,
    <trd>Kolophonium</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>colophony</trd>,
    <trd>resin</trd>,
    <trd>rosin</trd>
  </trdgrp>
  <trd lng="fr">colophane</trd>
  <trd lng="nl">hars</trd>
  <trdgrp lng="pt">
    <trd>breu</trd>,
    <trd>breu de colof&ocirc;nia</trd>,
    <trd>colof&ocirc;nia</trd>,    
    <trd>colof&ocirc;nio</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_n;&c_i;&c_f;&c_o;&c_l;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>explicit</trd>,
    <trd>kolof&oacute;n</trd>
  </trdgrp>
</drv>
</art>
