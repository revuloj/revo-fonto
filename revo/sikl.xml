<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sikl.xml,v 1.13 2023/09/03 18:39:29 revo Exp $">
<kap>
  <rad>sikl</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="sikl.0o">
  <kap><tld/>o</kap>
  <snc mrk="sikl.0o.BIB">
    <uzo tip="fak">BIB</uzo>
    <dif>
      Antikva okcidentazia <ref tip="lst" cel="unu.mezur0o" lst="voko:kvantoj">pezunuo</ref>
      (6&#x202F;&ndash;&#x202F;16 gramojn valora)
      kaj <ref tip="lst" cel="unu.mon0o.EKON" lst="voko:monunuoj">monunuo</ref>:
      <ekz>
        la tero valoras kvarcent <tld/>ojn
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 23:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        la viro prenis oran ringon, havantan la pezon de duono de <tld/>o, kaj du
        braceletojn por &scirc;iaj manoj, havantajn la pezon de dek <tld/>oj da oro
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 24:22</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_y;&c_k;&c_l;&c_mol;</trd>,
      <trd>&c_s;&c_ib;&c_k;&c_l;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sikl.0o.sxekelo">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="dif" cel="sxekel.0o.EKON">&Scirc;ekelo</ref>:
      <ekz>
        la israela mono estas la (nova) <tld/>o
        <fnt>
          <vrk>85-a Universala Kongreso de Esperanto ... 2000</vrk>,
          <lok><url ref="http://esperanto.org/uk2000/utinform.html">
            Utilaj informoj pri Israelo...</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_sh;&c_e;&c_k;&c_je;&c_l;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>sekel</trd>,
    <trd>&scaron;ekel</trd>
  </trdgrp>
  <trd lng="de">Schekel</trd>
  <trd lng="en">shekel</trd>
  <trd lng="fr">sicle</trd>
  <trd lng="it">siclo</trd>
  <trd lng="ja">&#x30B7;&#x30B1;&#x30EB;</trd>
  <trdgrp lng="pl">
    <trd>srebrnik</trd>,
    <trd>sykiel</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_i;&c_k;&c_l;</trd>
  <trd lng="sk">&scaron;ekel</trd>
  <trd lng="uk">&c_s;&c_i;&c_k;&c_l;&c_mol;</trd>
</drv>
  </art>

