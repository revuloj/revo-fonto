<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>sardel</rad>/o
</kap>
<drv mrk="sardel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="klupe.0edoj">klupeedoj</ref>
      (<trd lng="la">Engraulis</trd>), ofte malgrande konservata en oleo:
      <ekz>
        ili kunpremi&gcirc;is simile al <tld/>oj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">anchovy</trd>
  <trdgrp lng="de">
    <trd>Sardelle</trd>,
    <trd>Anchovis</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>anchoa</trd>, <trd>boquer&oacute;n</trd>
  </trdgrp>
  <trd lng="fr">anchois</trd>
  <trdgrp lng="it">
    <trd>acciughe</trd>, <trd>alice</trd>
  </trdgrp>
  <trd lng="nl">ansjovis</trd>
  <trdgrp lng="pt">
    <trd>enchova</trd>, <trd>biqueir&atilde;o</trd>
  </trdgrp>
</drv>
</art>

