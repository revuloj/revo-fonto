<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sardel.xml,v 1.24 2025/01/18 16:29:49 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sardel</rad>/o <fnt><bib>UV</bib></fnt>

</kap>
<drv mrk="sardel.0o">
  <kap><ofc>*</ofc><tld/>o, <tld var="e"/>o, <var><kap><rad var="e">engra&ubreve;l</rad>/o
  <fnt><bib>JED</bib></fnt></kap></var></kap>
  <snc mrk="sardel.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="lst" cel="fisx.mar0o" lst="voko:fi&scirc;oj">Marfi&scirc;o</ref>
      relative malgranda inter 10&nbsp;cm kaj 20&nbsp;cm, kun mallar&gcirc;a
      longa
      korpo, vivanta en grandaj svarmoj,
      kies unu specion (<trd lng="la">Engraulis encrasicolus</trd>)
      estas man&gcirc;ebla, sed havas iom akre amaran guston,
      pro kio &gcirc;i estas preparata kiel <ref cel="ancxov.0o">an&ccirc;ovo</ref>;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el la ordo <ref
      tip="malprt" cel="klupe.0oformaj">klupeoformaj</ref>
      (<trd lng="la">Engraulis</trd>):
      <ekz>
        ili kunpremi&gcirc;is simile al <tld/>oj
        <fnt>
          <aut>H. Heine, trad. L. L. Zamenhof</aut>
          <vrk>La Rabeno de Ba&hcirc;ara&hcirc;</vrk>,
          <lok>1929</lok>
        </fnt>;
      </ekz>
      <ekz>
        Gastono englutis unuan <tld/>on, kaj duan, kaj trian, sin interrompante nur por
        trinki
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekoka</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni faras la fi&scirc;farunon kaj fi&scirc;oleon &ccirc;efe el grasaj fi&scirc;aj specioj kiel el
        engra&ubreve;lo, haringo, sardino
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=87287"
              >Fi&scirc;faruno</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="ancxov.0o">an&ccirc;ovo</ref>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_ch;&c_o;&c_w;&c_s;</trd>
  <trd lng="bg">&c_h;&c_a;&c_m;&c_s;&c_i;&c_ja;</trd>
  <trd lng="cs">nasolen&aacute; an&ccaron;ovi&ccaron;ka</trd>
  <trdgrp lng="de">
    <trd>Sardelle</trd>,
    <trd>Anchovis</trd>
  </trdgrp>
  <trd lng="en">anchovy</trd>
  <trdgrp lng="es">
    <trd>anchoa</trd>, <trd>boquer&oacute;n</trd>
  </trdgrp>
  <trd lng="fr">anchois</trd>
  <trdgrp lng="hu">
    <trd>szardella</trd>, <trd>aj&oacute;ka</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>acciuga</trd>, <trd>alice</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#22633;&#28460;&#12369;&#12452;&#12527;&#12471;  <pr>&#12375;&#12362;&#12389;&#12369;&#12452;&#12527;&#12471;</pr></trd>,
    <trd>&#22633;&#28460;&#12369;&#12450;&#12531;&#12481;&#12519;&#12499;&#12540;  <pr>&#12375;&#12362;&#12389;&#12369;&#12450;&#12531;&#12481;&#12519;&#12499;&#12540;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>sardien</trd>, <trd>sardine</trd>
  </trdgrp>
  <trd lng="pl">sardela</trd>
  <trdgrp lng="pt">
    <trd>enchova</trd>,
    <trd>biqueir&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_ch;&c_o;&c_u;&c_s;</trd>,
    <trd>&c_h;&c_a;&c_m;&c_s;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">nasolen&eacute; o&ccaron;ko</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_a;&c_r;&c_d;&c_i;&c_n;&c_a; (&c_v; &c_o;&c_l;&c_ib;&c_ji;)</trd>,
    <trd>&c_t;&c_ju;&c_l;&c_mol;&c_k;&c_a;</trd>,
    <trd>&c_a;&c_n;&c_ch;&c_o;&c_u;&c_s; (&c_s;&c_o;&c_l;&c_o;&c_n;&c_ib;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#39927; <pr>t&iacute;</pr></trd>,
    <trd>&#40128; <pr>t&iacute;</pr></trd>
  </trdgrp>
</drv>
</art>
