<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sardel</rad>/o
</kap>
<drv mrk="sardel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="klupee.0oj">klupeedoj</ref>
      (<trd lng="la">Engraulis</trd>), ofte malgrande konservata en
      oleo:
      <ekz>
        ili kunpremi&gcirc;is simile al <tld/>oj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_ch;&c_o;&c_w;&c_s;</trd>
  <trd lng="bg">&c_h;&c_a;&c_m;&c_s;&c_i;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Sardelle</trd>,
    <trd>Anchovis</trd>
  </trdgrp>
  <trd lng="en">anchovy</trd>
  <trdgrp lng="es">
    <trd>anchoa</trd>, <trd>boquer&oacute;n</trd>
  </trdgrp>
  <trd lng="fr">anchois</trd>
  <trdgrp lng="hu">
    <trd>szardella</trd>, <trd>aj&oacute;ka</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>acciuga</trd>, <trd>alice</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>sardien</trd>, <trd>sardine</trd>
  </trdgrp>
  <trd lng="pl">sardela</trd>
  <trdgrp lng="pt">
    <trd>enchova</trd>,
    <trd>biqueir&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_ch;&c_o;&c_u;&c_s;</trd>,
    <trd>&c_h;&c_a;&c_m;&c_s;&c_a;</trd>
  </trdgrp>
</drv>
</art>
