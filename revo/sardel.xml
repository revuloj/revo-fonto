<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sardel.xml,v 1.21 2023/10/21 06:31:28 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sardel</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sardel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sardel.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="lst" cel="fisx.mar0o" lst="voko:fi&scirc;oj">Marfi&scirc;o</ref>
      relative malgranda inter 10&nbsp;cm kaj 20&nbsp;cm, kun mallar&gcirc;a
      longa 
      korpo, vivanta en grandaj svarmoj, 
      kies unu specion (<trd lng="la">Engraulis encrasicolus</trd>)
      estas man&gcirc;ebla, sed havas iom akre amaran guston,
      pro kio &gcirc;i estas preparata kiel <ref cel="ancxov.0o">an&ccirc;ovo</ref>;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el la ordo <ref tip="malprt" cel="klupe.0oformaj">klupeoformaj</ref>
      (<trd lng="la">Engraulis</trd>):
      <ekz>
        ili kunpremi&gcirc;is simile al <tld/>oj
        <fnt>
          <aut>H. Heine, trad. L. L. Zamenhof</aut>
          <vrk>La Rabeno de Ba&hcirc;ara&hcirc;</vrk>,
          <lok>1929</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_ch;&c_o;&c_w;&c_s;</trd>
  <trd lng="bg">&c_h;&c_a;&c_m;&c_s;&c_i;&c_ja;</trd>
  <trd lng="cs">nasolen&aacute; an&ccaron;ovi&ccaron;ka</trd>
  <trdgrp lng="de">
    <trd>Sardelle</trd>,
    <trd>Anchovis</trd>
  </trdgrp>
  <trd lng="en">anchovy</trd>
  <trdgrp lng="es">
    <trd>anchoa</trd>, <trd>boquer&oacute;n</trd>
  </trdgrp>
  <trd lng="fr">anchois</trd>
  <trdgrp lng="hu">
    <trd>szardella</trd>, <trd>aj&oacute;ka</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>acciuga</trd>, <trd>alice</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5869;&#x6F2C;&#x3051;&#x30A4;&#x30EF;&#x30B7;  <pr>&#x3057;&#x304A;&#x3065;&#x3051;&#x30A4;&#x30EF;&#x30B7;</pr></trd>,
    <trd>&#x5869;&#x6F2C;&#x3051;&#x30A2;&#x30F3;&#x30C1;&#x30E7;&#x30D3;&#x30FC;  <pr>&#x3057;&#x304A;&#x3065;&#x3051;&#x30A2;&#x30F3;&#x30C1;&#x30E7;&#x30D3;&#x30FC;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>sardien</trd>, <trd>sardine</trd>
  </trdgrp>
  <trd lng="pl">sardela</trd>
  <trdgrp lng="pt">
    <trd>enchova</trd>,
    <trd>biqueir&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_ch;&c_o;&c_u;&c_s;</trd>,
    <trd>&c_h;&c_a;&c_m;&c_s;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">nasolen&eacute; o&ccaron;ko</trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x430;&#x440;&#x434;&#x438;&#x43D;&#x430; (&#x432; &#x43E;&#x43B;&#x456;&#x457;)</trd>,
    <trd>&#x442;&#x44E;&#x43B;&#x44C;&#x43A;&#x430;</trd>,
    <trd>&#x430;&#x43D;&#x447;&#x43E;&#x443;&#x441; (&#x441;&#x43E;&#x43B;&#x43E;&#x43D;&#x456;)</trd>
  </trdgrp>
</drv>
</art>
