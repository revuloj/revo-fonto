<drv mrk="send.dis0i">
  <kap>dis<tld/>i</kap>
  <snc mrk="send.dis0i.KOMUNE">
    <dif>
      Irigi al &ccirc;iuj direktoj:
      <ekz>
        li dis<tld/>is &ccirc;iujn siajn filojn en &ccirc;iujn regionojn de <nom>Jehuda</nom>
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 11:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        li dis<tld/>is la kunvenintojn
        <fnt><bib>NT</bib>, <lok>&Ago; 19:40</lok></fnt>;
      </ekz>
      <ekz>
        li dis<tld/>is esploristojn al la malamiko
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        li e&ccirc; ne rimarkis la kolosan perukon de Tutmozis, kiu dis<tld/>is el si dek kvin
        parfumojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz><ekz>
        karboj de la fajrujo dis<tld/>is nur malfortan lumon
        <fnt><bib>ElektFab</bib>,
          <lok>pa&scirc;tistino de anseroj apud puto</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_s;&c_s;&c_y;&c_l;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_w;&c_s;&c_ju;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_r;&c_a;&c_s;&c_s;&c_y;&c_l;&c_k;&c_a;&c_j;)</klr></trd>
    </trdgrp>
    <trdgrp lng="it">
    <trd>diramare</trd>,
    <trd>diffondere</trd>
  </trdgrp>
  </snc>
  <snc mrk="send.dis0i.TEL">
    <uzo tip="fak">RAD</uzo>
    <uzo tip="fak">TEL</uzo>
    <dif>
      <ref tip="dif" cel="send.el0i.TEL">Elsendi</ref>:
      <ekz>
        dis<tld/>is la irlanda &scirc;tata televido filmon pri la meteologia fenomeno nomata
        &scirc;prucfluo
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2003/008161.html">Per flugiloj de malfacila
          vento</url></vrk>
          <bib>Monato</bib>
          <lok>2003/11, p. 14</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili dis<tld/>is siajn politikajn ideojn tra Interreto
        <fnt>
          <aut>pp</aut>
          <vrk><url ref="&Monato;/2009/009778.php">Yahoo! akuzita</url></vrk>
          <bib>Monato</bib>
          <lok>2009/04, p. 10</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; la dua mondmilito pluraj radioj en E&ubreve;ropo dis<tld/>is
        programojn en Esperanto
        <fnt>
          <aut>R. Sartor</aut>,
          <vrk><url ref="http://es.chinabroadcast.cn/1/2004/12/13/1@18383.htm">... el
          Argentino</url></vrk>,
          <lok>&Ccirc;ina Radio Internacia, 2004-12-13</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_je;&c_r;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_p;&c_r;&c_a;&c_z; &c_s;&c_y;&c_g;&c_n;&c_a;&c_l;)</klr></trd>,
      <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_mol;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">ausstrahlen</trd>
  </snc>
