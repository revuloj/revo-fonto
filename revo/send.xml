    <trd>vyslanec</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gesandter</trd>,
    <trd>Bote</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>envoy</trd>,
    <trd>bearer</trd>,
    <trd>messenger</trd>,
    <trd>representative</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>envoy&eacute;</trd>,
    <trd>&eacute;missaire</trd>
  </trdgrp>
  <trd lng="hu">k&uuml;ld&ouml;tt</trd>
  <trdgrp lng="id">
