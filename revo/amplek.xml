<drv mrk="amplek.0a">
  <kap><tld/>a</kap>
  <snc mrk="amplek.0a.iel">
    <dif>
      Iel rilata al <tld/>o:
      <ekz>
        kodoj povas esti malsame <tld/>aj
        <fnt>
          <bib>Monato</bib>
          <aut>Pejno S.</aut>,
          <vrk>Tekstotraktado</vrk>,
        </fnt>;
      </ekz>
      <ekz>
        mond<tld/>a organiza&jcirc;o
        <fnt>
          <bib>Monato</bib>
          <vrk>Sir Edmund Hillary: Vivo dedi&ccirc;e al la &scirc;erpoj</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la plen<tld/>a vero
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#x2116; 8-9</lok>
        </fnt>;
      </ekz>
      <ekz>
        hom<tld/>aj felbotoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i estus pla&ccirc;a se &scirc;ia tuta sinteno ne esprimus &ccirc;io<tld/>an indiferenton
        <fnt>
          <bib>ChB</bib>
          <lok>15</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_z;&c_mol;&c_m;&c_je;&c_r;&c_n;&c_y;</trd>
    <trdgrp lng="de">
      <trd>im <ind>Ausma&szlig;</ind> von</trd>,
      <trd>umfassend</trd>
    </trdgrp>
    <trd lng="en">dimensional</trd>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_m;&c_je;&c_r;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="amplek.0a.vasta">
    <dif>
      <ref tip="dif" cel="amplek.grand0a.KOMUNE">Grand<tld/>a</ref> en &ccirc;iuj siaj dimensioj:
      <ekz>
        <tld/>a botelo
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la domo estis ju pli alte des pli <tld/>a &ccirc;iuflanke
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 41:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        la temo estas tiel <tld/>a, ke kiom ajn multe mi parolus, mi ne klarigus e&ccirc; la
        centonon
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Pri la homaranismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        pli<tld/>i&gcirc;is la politika debato pri la temo
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk>Edzi&gcirc;oj anka&ubreve; por samseksaj paroj</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la inter&scirc;an&gcirc;oj multobli&gcirc;as spece kaj <tld/>i&gcirc;as teritorie
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a kono kaj &scirc;ato de la angla literaturo
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekzistas aferoj ekster lia mal<tld/>a filozofio
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        homoj rigard<klr>[as]</klr> aferon kun des pli da estimego, ju pli &gcirc;i estas
        komplikita, <tld/>a kaj malfacile-digestebla
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_ja;&c_l;&c_ib;&c_z;&c_n;&c_y;</trd>,
      <trd>&c_u;&c_s;&c_jo;&c_a;&c_h;&c_o;&c_p;&c_n;&c_y;</trd>,
      <trd>&c_sh;&c_y;&c_r;&c_o;&c_k;&c_a;&c_m;&c_a;&c_sh;&c_t;&c_a;&c_b;&c_n;&c_y;</trd>,
      <trd>&c_a;&c_b;&#x2019;&c_jo;&c_m;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="de">ausgedehnt</trd>
    <trdgrp lng="pl">
      <trd>obszerny</trd>,
      <trd>rozleg&lstroke;y</trd>,
      <trd>wielki</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>ob&scaron;&iacute;rn&yacute;</trd>,
    <trd>rozs&aacute;hl&yacute;</trd>
  </trdgrp>
  <trd lng="en">extensive</trd>
  <trd lng="fr">&eacute;tendu<klr> (vaste)</klr></trd>
  <trd lng="he">&mem;&#x5B7;&qof;&#x5B4;&yod;&fpe;</trd>
  <trdgrp lng="ja">
    <trd>&#x5E83;&#x5927;&#x306A;  <pr>&#x3053;&#x3046;&#x3060;&#x3044;&#x306A;</pr></trd>,
    <trd>&#x5E83;&#x7BC4;&#x56F2;&#x306E;  <pr>&#x3053;&#x3046;&#x306F;&#x3093;&#x3044;&#x306E;</pr></trd>,
    <trd>&#x5305;&#x62EC;&#x7684;&#x306A;  <pr>&#x307B;&#x3046;&#x304B;&#x3064;&#x3066;&#x304D;&#x306A;</pr></trd>
  </trdgrp>
  <trd lng="ro">larg</trd>
  <trdgrp lng="sk">
    <trd>objemn&yacute;</trd>,
    <trd>obsiahly</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5927;&#x898F;&#x6A21; <pr>d&agrave;gu&imacron;m&oacute;</pr></trd>,
    <trd>&#x5927;&#x89C4;&#x6A21; <pr>d&agrave;gu&imacron;m&oacute;</pr></trd>,
    <trd>&#x5E7F;&#x5927; <pr>gu&#x1CE;ngd&agrave;</pr></trd>,
    <trd>&#x5EE3;&#x5927; <pr>gu&#x1CE;ngd&agrave;</pr></trd>,
    <trd>&#x5E7F;&#x535A; <pr>gu&#x1CE;ngb&oacute;</pr></trd>,
    <trd>&#x5EE3;&#x535A; <pr>gu&#x1CE;ngb&oacute;</pr></trd>,
    <trd>&#x6C3E; <pr>f&agrave;n</pr></trd>,
    <trd>&#x5927;&#x898F;&#x6A21;&#x7684; <pr>d&agrave;gu&imacron;m&oacute;de</pr></trd>,
    <trd>&#x5927;&#x89C4;&#x6A21;&#x7684; <pr>d&agrave;gu&imacron;m&oacute;de</pr></trd>,
    <trd>&#x5E7F; <pr>gu&#x1CE;ng</pr></trd>,
    <trd>&#x5EE3; <pr>gu&#x1CE;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="amplek.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="amplek.0i.KOMUNE">
    <dif>
      Havi <tld/>on de; <ref cel="hav.en0i.enSi">enhavi</ref>, <ref cel="ten.en0i">enteni</ref>:
      <ekz>
        la domo kun la korto <tld/>as 372 kvadratajn metrojn
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la instruado estu fundamenta, lar&gcirc;a, plena, ke &gcirc;i <tld/>u &ccirc;iujn bran&ccirc;ojn de la
        koncernata objekto
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        miaj &gcirc;isnunaj provoj ne <tld/>is ja ankora&ubreve; la tutan rondon da diversaj homaj
        laboroj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;ielo ne povas Vin <tld/>i
        <fnt>
          <bib>MT</bib>
          <lok>II. kroniko 6:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne povas <tld/>i <klr>(kapti la signifon, tutkompreni)</klr> tiun penson
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia invito <tld/>as min
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj diman&ccirc;aj promenoj nuntempe <tld/>is anka&ubreve; la bovin-stalon
        <fnt>
          <bib>HsT</bib>
          <lok>Feli&ccirc;a homo</lok>
        </fnt>;
      </ekz>
      <ekz>
        Grandpolujo, la okcidenta parto de Polujo, <klr>[&#x2026;]</klr> <tld/>as anka&ubreve; la
        nunan prusan Provincon Poznanian
        <fnt>
          <bib>SrT</bib>
          <lok>Libro II.</lok>
        </fnt>;
      </ekz>
      <ekz>
        la re&gcirc;o <tld/>is per la rigardo kaj svingo de la sceptro la vicon de la &jcirc;us
        alparolitaj viroj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiel oni per unu ekrigardo <tld/>as la tutan sennombran svarmon de la steloj
        <fnt>
          <bib>Fab3</bib>
          <lok>En la lasta tago</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pren.cxirkaux0i">&ccirc;irka&ubreve;preni</ref>,
      <ref cel="spac.en0i">enspaci</ref>,
      <ref cel="volv.en0i">envolvi</ref>,
      <ref cel="etend.0igxi">etendi&gcirc;i</ref>,
      <ref cel="okup.0i.lokigxi">okupi<sncref/></ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#x5E60; <pr>h&umacron;</pr></trd>,
    <trd>&#x5E6A; <pr>m&eacute;ng</pr></trd>
  </trdgrp>
</snc>
<trdgrp lng="be">
  <trd>&c_u;&c_t;&c_r;&c_y;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_u; &c_s;&c_a;&c_b;&c_je;)</klr></trd>,
  <trd>&c_a;&c_h;&c_o;&c_p;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
</trdgrp>
  <trdgrp lng="cs">
    <trd>obsahovat</trd>,
    <trd>zahrnovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>umfassen</trd>,
    <trd>einschlie&szlig;en</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>extend to</trd>,
    <trd>include</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>abarcar</trd>,
    <trd>incluir</trd>,
    <trd>ampliar a</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>comporter</trd>,
    <trd>comprendre</trd>,
    <trd>embrasser</trd>
  </trdgrp>
  <trd lng="he">&lamed;&he;&qof;&yod;&fpe;</trd>
  <trdgrp lng="ja">
    <trd>&#xFF5E;<ind>&#x306E;&#x5927;&#x304D;&#x3055;&#x304C;&#x3042;&#x308B;</ind>
    <pr>&#x306E;&#x304A;&#x304A;&#x304D;&#x3055;&#x304C;&#x3042;&#x308B;</pr></trd>,
    <trd>&#x5305;&#x542B;&#x3059;&#x308B; <pr>&#x307B;&#x3046;&#x304C;&#x3093;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">omvatten</trd>
  <trdgrp lng="pl">
    <trd>obejmowa&cacute;</trd>,
    <trd>ogarnia&cacute;</trd>,
    <trd>zawiera&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>extender-se at&eacute;</trd>,
    <trd>abranger</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_h;&c_v;&c_a;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>ma&tcaron; rozsah</trd>,
    <trd>obsahova&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="amplek.grand0a">
  <kap>grand<tld/>a, <var><kap>mult<tld/>a</kap></var></kap>
  <snc mrk="amplek.grand0a.KOMUNE">
    <dif>
      Havanta grandan <tld/>on,
      <ref tip="vid" cel="vast.0a">vasta</ref>:
      <ekz>
        bonvolu doni al li tiun grand<tld/>an leteron
        <fnt>
          <aut>H. Vallienne</aut>,
          <vrk>&Ccirc;u li?</vrk>,
          <lok>1908</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u tio ne estas grand<tld/>a propagando
        <fnt>
          <aut>E. Lanti</aut>,
