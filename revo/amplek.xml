<drv mrk="amplek.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="amplek.0o.KOMUNE">
    <dif>
      Dimensio, grado de grandeco, konsiderata rilate al tio, kion
      &gcirc;i enhavas:
      <ekz>
        bildo la&ubreve; natura <tld/>o;
      </ekz>
      <ekz>
        <tld/>o de katastrofo, organizo, krizo, &jcirc;eto,
        balanci&gcirc;o, signifo, ideo;
      </ekz>
      <ekz>
        tiu principo havas pli grandan <tld/>on ol nure juran;
      </ekz>
      <ekz>
        vasta <tld/>o da farita lernado<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="spac.0o">spaco</ref>,
      <ref cel="volume.0o">volumeno</ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#22823;&#x5C0F;&#x7BC4;&#x570D; <pr>d&#xE0;xi&#x1CE;of&#xE0;nw&#xE9;i</pr></trd>,
    <trd>&#22823;&#x5C0F;&#33539;&#x56F4; <pr>d&#xE0;xi&#x1CE;of&#xE0;nw&#xE9;i</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_m;&c_je;&c_r;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_ib;&c_ch;&c_y;&c_n;&c_ja;</trd>,
    <trd>&c_a;&c_b;'&c_jo;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dosah</trd>,
    <trd>p&aacute;smo</trd>,
    <trd>rozloha</trd>,
    <trd>rozmez&iacute;</trd>,
    <trd>rozsah</trd>,
    <trd>&scaron;&iacute;&rcaron;e</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ausdehnung <klr>(Gr&ouml;&szlig;e)</klr></trd>,
    <trd>Ausma&szlig;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>extent</trd>,
    <trd>scope</trd>,
    <trd>magnitude</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>amplitud</trd>,
    <trd>extensi&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ampleur</trd>,
    <trd>&eacute;tendue</trd>,
    <trd>port&eacute;e</trd>,
    <trd>taille</trd>
  </trdgrp>
  <trd lng="he">&#x5EA;&#x5B0;&#x5D7;&#x5D5;&#x5BC;&#x5DD;</trd>
  <trdgrp lng="ja">
    <trd>&#22823;&#x304D;&#12373;  <pr>&#x304A;&#x304A;&#x304D;&#12373;</pr></trd>,
    <trd>&#x5E83;&#x304C;&#x308A;  <pr>&#12402;&#x308D;&#x304C;&#x308A;</pr></trd>,
    <trd>&#22806;&#x5EF6;  <pr>&#x304C;&#12356;&#12360;&#12435;</pr></trd>,
    <trd>&#20998;&#x5E03;&#x7BC4;&#x56F2;  <pr>&#12406;&#12435;&#12407;&#x306F;&#12435;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">omvang</trd>
  <trdgrp lng="pl">
    <trd>rozmiar</trd>,
    <trd>zasi&eogonek;g</trd>,
    <trd>zakres</trd>,
    <trd>obj&eogonek;to&sacute;&cacute;</trd>,
    <trd>rozpi&eogonek;to&sacute;&cacute;</trd>,
    <trd>wielko&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>amplitude</trd>,
    <trd>amplid&atilde;o</trd>
    <trd>dimens&atilde;o</trd>
    <trd>extens&atilde;o</trd>
    <trd>grandeza</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_l;&c_i;&c_ch;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_m;&c_je;&c_r;</trd>,
    <trd>&c_o;&c_b;&c_malmol;&c_jo;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dosah</trd>,
    <trd>p&aacute;smo</trd>,
    <trd>rozsah</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43E;&c_b;&c_s;&#x44F;&c_g;</trd>,
    <trd>&c_r;&#x43E;&c_z;&#x43C;&c_ib;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="amplek.0a">
  <kap><tld/>a</kap>
  <snc mrk="amplek.0a.iel">
    <dif>
      Iel rilata al <tld/>o:
      <ekz>
        kodoj povas esti malsame <tld/>aj
        <fnt>
          <bib>Monato</bib>
          <aut>Pejno S.</aut>,
          <vrk>Tekstotraktado</vrk>,
        </fnt>;
      </ekz>
      <ekz>
        sam<tld/>aj grandoj.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_z;&c_mol;&c_m;&c_je;&c_r;&c_n;&c_y;</trd>
    <trd lng="en">dimensional</trd>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_m;&c_je;&c_r;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="amplek.0a.vasta">
    <dif>
      <ref tip="dif" cel="amplek.grand0a.KOMUNE">Grand<tld/>a</ref>:
      <ekz>
        <tld/>a verko, spirito;
      </ekz>
      <ekz>
        la arto de verkado <klr>(...)</klr> nepre postulas certan
        sufi&ccirc;e vastan <tld/>on da farita lernado, certan sufi&ccirc;e
        <tld/>an sciadon komune sciencan kaj speciale teknikan
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz> la agado de
        nia societo tagon post tago dis<tld/>i&gcirc;as.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_ja;&c_l;&c_ib;&c_z;&c_n;&c_y;</trd>,
      <trd>&c_u;&c_s;&c_jo;&c_a;&c_h;&c_o;&c_p;&c_n;&c_y;</trd>,
      <trd>&c_sh;&c_y;&c_r;&c_o;&c_k;&c_a;&c_m;&c_a;&c_sh;&c_t;&c_a;&c_b;&c_n;&c_y;</trd>,
      <trd>&c_a;&c_b;&#8217;&c_jo;&c_m;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>obszerny</trd>,
      <trd>rozleg&lstroke;y</trd>,
      <trd>wielki</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>ob&scaron;&iacute;rn&yacute;</trd>,
    <trd>rozs&aacute;hl&yacute;</trd>
  </trdgrp>
  <trd lng="en">extensive</trd>
  <trd lng="fr">&eacute;tendu<klr> (vaste)</klr></trd>
  <trd lng="he">&#x5DE;&#x5B7;&#x5E7;&#x5B4;&#x5D9;&#x5E3;</trd>
  <trdgrp lng="ja">
    <trd>&#x5E83;&#22823;&#x306A;  <pr>&#12371;&#12358;&#12384;&#12356;&#x306A;</pr></trd>,
    <trd>&#x5E83;&#x7BC4;&#x56F2;&#x306E;  <pr>&#12371;&#12358;&#x306F;&#12435;&#12356;&#x306E;</pr></trd>,
    <trd>&#21253;&#x62EC;&#30340;&#x306A;  <pr>&#x307B;&#12358;&#x304B;&#12388;&#12390;&#x304D;&#x306A;</pr></trd>
  </trdgrp>
  <trd lng="ro">larg</trd>
  <trdgrp lng="sk">
    <trd>objemn&yacute;</trd>,
    <trd>obsiahly</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#22823;&#x898F;&#x6A21; <pr>d&#xE0;gu&#x12B;m&#xF3;</pr></trd>,
    <trd>&#22823;&#x89C4;&#x6A21; <pr>d&#xE0;gu&#x12B;m&#xF3;</pr></trd>,
    <trd>&#x5E7F;&#22823; <pr>gu&#x1CE;ngd&#xE0;</pr></trd>,
    <trd>&#x5EE3;&#22823; <pr>gu&#x1CE;ngd&#xE0;</pr></trd>,
    <trd>&#x5E7F;&#x535A; <pr>gu&#x1CE;ngb&#xF3;</pr></trd>,
    <trd>&#x5EE3;&#x535A; <pr>gu&#x1CE;ngb&#xF3;</pr></trd>,
    <trd>&#x6C3E; <pr>f&#xE0;n</pr></trd>,
    <trd>&#22823;&#x898F;&#x6A21;&#30340; <pr>d&#xE0;gu&#x12B;m&#xF3;de</pr></trd>,
    <trd>&#22823;&#x89C4;&#x6A21;&#30340; <pr>d&#xE0;gu&#x12B;m&#xF3;de</pr></trd>,
    <trd>&#x5E7F; <pr>gu&#x1CE;ng</pr></trd>,
    <trd>&#x5EE3; <pr>gu&#x1CE;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="amplek.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="amplek.0i.KOMUNE">
    <dif>
      Havi <tld/>on de; enhavi, enteni:
      <ekz>
        la &ccirc;ielo ne povas Vin
        <tld/>i
        <fnt>
          <bib>MT</bib>
          <lok>II. kroniko 6:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        la registara deklaro <tld/>as &ccirc;iujn nunajn
        politikajn demandojn;
      </ekz>
      <ekz>
        ni ordinare kunlegas tutan serion da vortoj <tld/>itan per
        unu vido.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pren.cxirkaux0i">&ccirc;irka&ubreve;preni</ref>,
      <ref cel="hav.en0i">enhavi</ref>,
      <ref cel="spac.en0i">enspaci</ref>,
      <ref cel="ten.en0i">enteni</ref>,
      <ref cel="volv.en0i">envolvi</ref>,
      <ref cel="etend.0igxi">etendi&gcirc;i</ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#x5E60; <pr>h&#x16B;</pr></trd>,
    <trd>&#x5E6A; <pr>m&#xE9;ng</pr></trd>
  </trdgrp>
</snc>
<trdgrp lng="be">
  <trd>&c_u;&c_t;&c_r;&c_y;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_u; &c_s;&c_a;&c_b;&c_je;)</klr></trd>,
  <trd>&c_a;&c_h;&c_o;&c_p;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
</trdgrp>
  <trdgrp lng="cs">
    <trd>obsahovat</trd>,
    <trd>zahrnovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>umfassen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>extend to</trd>,
    <trd>include</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>abarcar</trd>,
    <trd>incluir</trd>,
    <trd>ampliar a</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>comporter</trd>,
    <trd>comprendre</trd>,
    <trd>embrasser</trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5D4;&#x5E7;&#x5D9;&#x5E3;</trd>
  <trdgrp lng="ja">
    <trd>&#xFF5E;<ind>&#x306E;&#22823;&#x304D;&#12373;&#x304C;&#12354;&#x308B;</ind>
    <pr>&#x306E;&#x304A;&#x304A;&#x304D;&#12373;&#x304C;&#12354;&#x308B;</pr></trd>,
    <trd>&#21253;&#x542B;&#12377;&#x308B; <pr>&#x307B;&#12358;&#x304C;&#12435;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">omvatten</trd>
  <trdgrp lng="pl">
    <trd>obejmowa&cacute;</trd>,
    <trd>ogarnia&cacute;</trd>,
    <trd>zawiera&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>extender-se at&eacute;</trd>,
    <trd>abranger</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_h;&c_v;&c_a;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>ma&tcaron; rozsah</trd>,
    <trd>obsahova&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="amplek.grand0a">
  <kap>grand<tld/>a, <var><kap>mult<tld/>a</kap></var></kap>
  <snc mrk="amplek.grand0a.KOMUNE">
    <dif>
      Havanta grandan <tld/>on,
      <ref tip="vid" cel="vast.0a">vasta</ref>:
      <ekz>
        bonvolu doni al li tiun grand<tld/>an leteron
        <fnt>
          <aut>H. Vallienne</aut>,
          <vrk>&Ccirc;u li?</vrk>,
          <lok>1908</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u tio ne estas grand<tld/>a propagando?
        <fnt>
          <aut>E. Lanti</aut>,
          <vrk>Publika letero al Romen Rolan</vrk>
        </fnt>;
      </ekz>
      <ekz>
        ili eniris <klr>...</klr> korton en la mezo de la
        mult<tld/>a templa konstrua&jcirc;aro
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 11a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#22823;&#x7BC4;&#x570D; <pr>d&#xE0;f&#xE0;nw&#xE9;i</pr></trd>,
    <trd>&#22823;&#33539;&#x56F4; <pr>d&#xE0;f&#xE0;nw&#xE9;i</pr></trd>,
    <trd>&#x535A; <pr>b&#xF3;</pr></trd>,
    <trd>&#22823;&#x800C;&#x8A00;&#x4E4B; <pr>d&#xE0;&#xE9;ry&#xE1;nzh&#x12B;</pr></trd>
  </trdgrp>
</snc>
    <trdgrp lng="be">
      <trd>&c_v;&c_ja;&c_l;&c_ib;&c_z;&c_n;&c_y;</trd>,
      <trd>&c_u;&c_s;&c_jo;&c_a;&c_h;&c_o;&c_p;&c_n;&c_y;</trd>,
      <trd>&c_sh;&c_y;&c_r;&c_o;&c_k;&c_a;&c_m;&c_a;&c_sh;&c_t;&c_a;&c_b;&c_n;&c_y;</trd>,
      <trd>&c_a;&c_b;&#8217;&c_jo;&c_m;&c_n;&c_y;</trd>
    </trdgrp>
   <trd lng="cs">objemn&yacute;</trd>
  <trdgrp lng="de">
    <trd>ausgedehnt</trd>,
    <trd><klr>von </klr>gro&szlig;em <ind>Ausma&szlig;</ind></trd>,
    <trd>weitr&auml;umig</trd>,
    <trd>extensiv</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>comprehensive</trd>,
    <trd>extensive</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>amplio</trd>,
    <trd>amplia</trd>,
    <trd>extenso</trd>,
    <trd>extensa</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ample</trd>,
    <trd>vaste</trd>
  </trdgrp>
  <trd lng="nl">omvangrijk</trd>
  <trdgrp lng="pl">
    <trd>obszerny</trd>,
    <trd>rozleg&lstroke;y</trd>,
    <trd>wielki</trd>
  </trdgrp>
  <trd lng="pt">amplo</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_b;&c_sh;&c_i;&c_r;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">objemn&yacute;</trd>
</drv>

<drv mrk="amplek.korp0a">
  <kap>korp<tld/>a</kap>
  <snc mrk="amplek.korp0a.KOMUNE">
    <dif>
      Vastkorpa,
      <ref tip="sin" cel="korpul.0a.KOMUNE">korpulenta</ref>:
      <ekz>
        al la leona ka&gcirc;o per elasta danca pa&scirc;o
        trotas korp<tld/>a viro
        <fnt>
          <aut>S. Bubeni&ccaron;</aut>,
          <vrk>Sub Cirkotendo</vrk>,
          <lok>1969</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x4FBF;&#x4FBF; <pr>pi&#xE1;npi&#xE1;n</pr></trd>
</snc>
  <trdgrp lng="be">
     <trd>&c_m;&c_a;&c_zh;&c_n;&c_y;</trd>,
     <trd>&c_t;&c_l;&c_u;&c_s;&c_t;&c_y;</trd>,
     <trd>&c_g;&c_l;&c_a;&c_d;&c_k;&c_ib;</trd>,
     <trd>&c_p;&c_o;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>korpulent</trd>,
     <trd>beleibt</trd>,
     <trd>wohlbeleibt</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>corpulent</trd>,
    <trd>fat</trd>,
    <trd>fleshy</trd>,
    <trd>obese</trd>,
    <trd>stout</trd>
  </trdgrp>
  <trd lng="es">corpulento</trd>
  <trd lng="fr">corpulent</trd>
  <trdgrp lng="hu">
     <trd>testes</trd>,
     <trd>korpulens</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>corpulent</trd>,
    <trd>zwaarlijvig</trd>
    <trd>dik</trd>,
    <trd>gezet</trd>,
    <trd>vlezig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>korpulentny</trd>,
    <trd>wielki</trd>
  </trdgrp>
  <trd lng="pt">corpulento</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_r;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_u;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
