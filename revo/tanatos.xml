<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: tanatos.xml,v 1.4 2024/05/10 08:31:29 afido Exp $">
    <kap>
      <rad>Tanatos</rad>/o
    </kap>

<drv mrk="tanatos.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIT</uzo>
  <snc>
    <dif>
      Helena <ref tip="super" cel="dajmon.0o">dajmono</ref> de morto:
      <ekz>
        li volus ankora&ubreve; doni sin al Hipnoso, anta&ubreve; ol <tld/>o dormigos lin por &ccirc;iam
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LXXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        Gotika kino arigas filmojn influitajn de gotika literaturo <klr>[&#x2026;]</klr>,
        kunigante Eroson kaj <tld/>on, realon kaj fabelaron
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=254769">Gotika kino</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="mort.0o.personigita">morto<sncref/></ref>
    <trd lng="be">&c_T;&c_a;&c_n;&c_a;&c_t;&c_a;&c_s;</trd>
    <trd lng="de">Thanatos</trd>
    <trd lng="en">Thanatos</trd>
    <trd lng="grc">&Theta;&alfa_ton;&ny;&alfa;&tau;&omikron;&sigma_fina;</trd>
    <trd lng="pl">Tanatos</trd>
  </snc>
  <trdgrp lng="zh">
    <trd>&#x5854;&#x7D0D;&#x8A17;&#x65AF; <pr>t&#x1CE;n&#xE0;tu&#x14D;s&#x12B;</pr></trd>,
    <trd>&#x5854;&#x7EB3;&#x6258;&#x65AF; <pr>t&#x1CE;n&#xE0;tu&#x14D;s&#x12B;</pr></trd>,
    <trd>&#x6492;&#x90A3;&#x7279;&#x65AF; <pr>s&#x1CE;n&#xE0;t&#xE8;s&#x12B;</pr></trd>
  </trdgrp>
</drv>
  </art>

