<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>enga&gcirc;</rad>/i
</kap>
<drv mrk="engagx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Dungi distriston a&ubreve; artiston por limigita tempo.
    </dif>
    <trdgrp lng="es">
      <trd>fichar</trd>,
      <trd>inscribir</trd>,
      <trd>reclutar</trd>,
      <trd>contratar</trd>
    </trdgrp>
    <trd lng="fr">enr&ocirc;ler</trd>
    <trd lng="hu">szerz&odblac;dtet <klr
    tip="ind">(m&udblac;v&eacute;szt)</klr></trd>
    <trdgrp lng="pl">
      <trd>dawa&cacute; <ind>anga&zdot;</ind></trd>,
      <trd>anga&zdot;owa&cacute;<klr> (kogo&sacute;)</klr></trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Igi iun partopreni en io.
    </dif>
    <trdgrp lng="es">
      <trd>implicar</trd>,
      <trd>comprometer</trd>
    </trdgrp>
    <trd lng="hu">bevon <klr tip="ind">(dologba)</klr></trd>
    <trd lng="pl">anga&zdot;owa&cacute;<klr> (kogo&sacute;, w
    co&sacute;)</klr></trd>
  </snc>
  <trdgrp lng="es">
    <trd>involucrar</trd>
  </trdgrp>
  <trd lng="fr">engager</trd>
  <trd lng="no">engasjere</trd>
  <trd lng="pt">engajar</trd>
  <trdgrp lng="th">
    <trd>&#3592;&#3657;&#3634;&#3591;</trd>,
    <trd>&#3594;&#3633;&#3585;&#3594;&#3623;&#3609;</trd>
  </trdgrp>
</drv>

<drv mrk="engagx.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Proprainiciate aktivi a&ubreve; defendi vidpunkton pri afero (ofte
      politika) kvankam tio ne estas deviga.
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd>implicarse</trd>,
    <trd>comprometerse</trd>,
    <trd>enfrascarse</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'engager</trd>,
    <trd>s'investir</trd>
  </trdgrp>
  <trd lng="hu">elk&ouml;telezi mag&aacute;t</trd>
  <trd lng="pl">anga&zdot;owa&cacute; si&eogonek;<klr> (w
  co&sacute;)</klr></trd>
</drv>
</art>

