<art mrk="$Id: omlet.xml,v 1.22 2022/03/24 15:12:21 revo Exp $">
<kap>
  <rad>omlet</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="omlet.0o">
  <kap><tld/>o</kap>
  <snc mrk="omlet.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="dif" cel="ov.kirl0ajxo.KUI">Kirlova&jcirc;o</ref>:
      <ekz>
        <tld/>o kun froma&gcirc;o;
      </ekz>
      <ekz>
        renversu la <tld/>on
        <fnt>
          <vrk><url ref="http://www.espero.com.cn/2017-11/03/content_50051535.htm">Omleto el Italio</url></vrk>
          <lok>espero.com.cn</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;macis kaj kraketis <tld/>o sur la forno
        <fnt>
          <aut>P. Thorsen</aut>,
          <vrk><url ref="http://www.poulthorsen.dk/verywell.htm">
          Very well, Hortensia</url></vrk>,
          <lok>en: Stokita vino, 1995</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_l;&c_je;&c_t;</trd>
  <trd lng="bg">&c_o;&c_m;&c_l;&c_je;&c_t;</trd>
  <trd lng="ca">truita</trd>
  <trdgrp lng="cs">
    <trd>amoleta</trd>,
    <trd>omeleta</trd>
  </trdgrp>
  <trd lng="de">Omelett</trd>
  <trd lng="el">&omikron;&my;&epsilon;&lambda;&epsilon_ton;&tau;&tau;&alfa;</trd>
  <trd lng="en">omelette</trd>
  <trdgrp lng="es">
    <trd>tortilla<klr> (de huevo)</klr></trd>
  </trdgrp>
  <trd lng="fr">omelette</trd>
  <trdgrp lng="hu">
    <trd>toj&aacute;slep&eacute;ny</trd>,
    <trd>omlett</trd></trdgrp>
  <trd lng="nl">omelet</trd>
  <trd lng="pl">omlet</trd>
  <trdgrp lng="pt">
    <trd>omelete</trd>,
    <trd>omeleta</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_m;&c_l;&c_je;&c_t;</trd>
  <trd lng="sk">omeleta</trd>
  <trd lng="sv">omelett</trd>
  <trd lng="uk">&#x43E;&#x43C;&#x43B;&#x435;&#x442;</trd>
</drv>
