<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>flirt</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="flirt.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="flirt.0i.MOV">
    <dif>
      Delikate movi&gcirc;i en la aero tien kaj reen, flugi &ccirc;irka&ubreve;
      io tien kaj returnen:
      <ekz>
	la papilioj <tld/>is &ccirc;irka&ubreve; floroj<fnt>B</fnt>;
      </ekz>
      <ekz>
	la flagoj <tld/>as en la aero, en la vento<fnt>Z</fnt>;
      </ekz>
      <ekz>
	gaje <tld/>anta pluma&jcirc;o
	<fnt><bib>Fab2</bib>, <lok>Malgranda Tuk</lok></fnt>;
      </ekz>
      <ekz>
	gracie <tld/>antaj dancistinoj;
      </ekz>
      <ekz>
	tra la bran&ccirc;oj <tld/>adis sunradio;
      </ekz>
      <ekz>
	la flamoj <tld/>is
	<klr>(<ref cel="flagr.0i">flagris</ref>)</klr>;
      </ekz>
      <ekz>
	lasi <tld/>i siajn rigardojn.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_y;&c_r;&c_h;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_l;&c_jo;&c_t;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">flattern</trd>
    <trdgrp lng="es">
      <trd>revolotear</trd>,
      <trd>ondear</trd>,
      <trd>aletear</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>virevolter</trd>,
      <trd>voleter</trd>,
      <trd>voltiger</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>repdes</trd>,
      <trd>lobog</trd>
    </trdgrp>
    <trd lng="nl">wapperen</trd>
  <trdgrp lng="pt">
    <trd>esvoa&ccedil;ar</trd>,
    <trd>flutuar</trd>,
    <trd>tremular</trd>,
    <trd>adejar</trd>,
    <trd>voejar</trd>,
    <trd>borboletear</trd>
  </trdgrp>
    <trd lng="ru">&c_p;&c_o;&c_r;&c_h;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="flirt.0i.FIG">
    <dif>
      Amindumi kvaza&ubreve; flugetante de unu al alia kaj ne celante
      edzi&gcirc;on:
      <ekz>
	&scirc;i havas sian preferitan <tld/>anton.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="koket.0i">koketi</ref>,
      <ref cel="papili.0i">papilii</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_f;&c_l;&c_ib;&c_r;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_l;&c_ja;&c_c;&c_a;&c_c;&c_c;&c_a;</trd>
    </trdgrp>
    <trd lng="bg">&c_f;&c_l;&c_i;&c_r;&c_t;&c_u;&c_v;&c_a;&c_m;</trd>
    <trd lng="de">flirten</trd>
    <trdgrp lng="es">
      <trd>coquetear</trd>,
      <trd>flirtear</trd>,
      <trd>ligar</trd> 
    </trdgrp> 
    <trdgrp lng="fr">
      <trd>flirter</trd>,
      <trd>coqueter<klr tip="ind"> (faire la cour)</klr></trd>,
      <trd>faire la <ind>cour</ind></trd>,
      <trd>draguer<klr tip="ind"> (qn)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kac&eacute;rkodik</trd>,
      <trd>fl&ouml;rt&ouml;l</trd>
    </trdgrp>
    <trd lng="nl">flirten</trd>   
    <trdgrp lng="pt">
      <trd>flertar</trd>,
      <trd>namorar</trd>,
      <trd>namoricar</trd>,
      <trd>galantear</trd>,
      <trd>fazer a <ind>corte</ind> a</trd>
    </trdgrp>
    <trd lng="ru">&c_f;&c_l;&c_i;&c_r;&c_t;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>poletovat</trd>,
    <trd>třepetat</trd>,
    <trd>vlát</trd>
  </trdgrp>
  <trd lng="en">flirt</trd>
  <trdgrp lng="sk">
    <trd>poletovať</trd>,
    <trd>trepotať</trd>,
    <trd>viať</trd>
  </trdgrp>
</drv>

<drv mrk="flirt.0ema">
  <kap><tld/>ema<fnt>Z</fnt></kap>
  <snc mrk="flirt.0ema.MOV">
    <dif>
      Volonte <tld/>anta.
    </dif>
  </snc>
  <snc mrk="flirt.0ema.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malkonstanta, malserioza:
      <ekz>
        <tld/>ema adoranto de inoj plej diversaj
        <fnt>
          <aut>J. Racine, trad. G. Lagrange</aut>,
          <vrk>Fedra</vrk>,
          <lok>1991</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>poletavý</trd>,
    <trd>záletný</trd>
  </trdgrp>
  <trd lng="de">flatterhaft</trd>
  <trdgrp lng="fr">
    <trd>dragueur<klr tip="ind"> (adj.)</klr></trd>,
    <trd>flirteur</trd>,
    <trd>frivole</trd>,
    <trd>inconstant</trd>,
    <trd>versatile</trd>
  </trdgrp>
  <trd lng="nl">flirtziek</trd>
  <trdgrp lng="sk">
    <trd>prelietavý</trd>,
    <trd>záletný</trd>
  </trdgrp>
</drv>

<drv mrk="flirt.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="F"/>i &ccirc;irka&ubreve;:
      <ekz>
	&scirc;i estas &ccirc;irka&ubreve;<tld/>ata de siaj longaj haroj kaj de
	sia verda tuko<fnt>Z</fnt>;
      </ekz>
      <ekz>
	&ccirc;irka&ubreve;flatadi kaj <ind>&ccirc;irka&ubreve;<tld/>adi</ind> iun
	<fnt><bib>PrV</bib></fnt>.
	<trdgrp lng="fr">
	  <trd>poursuivre<klr tip="ind"> (de ses assiduit&eacute;s)</klr></trd>,
	  <trd>&ecirc;tre <ind>empress&eacute;</ind><klr tip="ind"> (aupr&egrave;s
de)</klr></trd>
	</trdgrp>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>umflattern</trd>,
    <trd>umflirten</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>virevolter<klr tip="ind"> (tout autour de)</klr></trd>,
    <trd>voleter<klr tip="ind"> (tout autour de)</klr></trd>,
    <trd>voltiger<klr tip="ind"> (tout autour de)</klr></trd>,
  </trdgrp>
</drv>
</art>
