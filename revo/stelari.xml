<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stelari.xml,v 1.25 2024/03/25 07:28:36 revo Exp $">
<kap>
  <rad>stelari</rad>/o
</kap>

<drv mrk="stelari.0o">
  <kap><tld/>o</kap>
  <snc mrk="stelari.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="kariof1.0oj">kariofilacoj</ref>
      (<trd lng="la">Stellaria</trd>):
      <ekz>
        estas menciita nur unu arbospecio (la kverko) kaj du plantospecioj, nome <tld/>o kaj elimo
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2004/008323.html">Le&gcirc;oj landon prosperigas</url></vrk>
          <lok>2004/07, p. 27</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o: <klr>[&#x2026;]</klr> eblas man&gcirc;i la foliojn krude kaj 
        kuirite (3 minutoj sufi&ccirc;as), ekz. en salatoj, supo 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=157917">Stelario</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/5/55/Stellaria_holostea_a1.jpg/640px-Stellaria_holostea_a1.jpg" prm="CC     BY-SA 4.0"><ind>granda <tld/>o</ind> (Stellaria holostea), Gromnik, Polando

         <fnt>
           <aut>Jerzy Opio&lstroke;a, propra verko</aut>
           <lok><url ref="&WCW;/File:Stellaria_holostea_a1.jpg">&WCW;/File:Stellaria_holostea_a1.jpg</url></lok>
         </fnt>
      <trd lng="ca">r&egrave;vola</trd>
      <trd lng="de">Echte Sternmiere</trd>
      <trdgrp lng="en">
        <trd>Stellaria holostea</trd>,
        <trd>greater stitchwort</trd>
      </trdgrp>
      <trd lng="fa">&f_g;&a_n;&a_d;&a_m;&f_k; &a_z;&a_n;&f_g;&a_w;&a_l;&a_h;&#x200C;&a_A;&f_y;</trd>
      <trd lng="la">Stellaria holostea</trd>
      <trd lng="hu">olocs&aacute;ncsillagh&uacute;r</trd>
      <trd lng="nl">grote muur<klr tip="ind"> (plant)</klr></trd>
      <trd lng="pl">gwiazdnica wielkokwiatowa</trd>
    </bld>
    <trdgrp lng="en">
      <trd>Stellaria</trd>,
      <trd>chickweed</trd>, <trd>stitchwort</trd>
    </trdgrp>
  </snc>
  <trd lng="ca">g&egrave;nere Stellaria</trd>
  <trd lng="be">&c_z;&c_o;&c_r;&c_k;&c_a;&c_w;&c_k;&c_a;</trd>
  <trd lng="de">Sternmiere</trd>
  <trd lng="fa">&f_g;&a_n;&a_d;&a_m;&f_k;</trd>
  <trd lng="hu">csillagh&uacute;r</trd>
  <trd lng="nl">muur<klr tip="ind"> (plant)</klr></trd>
  <trd lng="pl">gwiazdnica</trd>
  <trd lng="ru">&c_z;&c_v;&c_je;&c_z;&c_d;&c_ch;&c_a;&c_t;&c_k;&c_a;</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_ib;&c_r;&#x43E;&c_ch;&#x43D;&c_i;&#x43A;</trd>,
    <trd>&#x43C;&#x43E;&#x43A;&c_r;&c_je;&c_c;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="stelari.meza0o">
  <kap>meza <tld/>o</kap>
  <snc mrk="stelari.meza0o.BOT">
  <uzo tip="fak">BOT</uzo>
    <dif>
      Specio de <ref tip="super" cel="stelari.0o.BOT">stelario</ref>
      malgranda, ku&scirc;a sur la grundo, kun nete dupartaj petaloj,
      komuna en kampoj kaj la&ubreve; vojoj de mezvarmaj landoj.
    </dif>
    <bld lok="&WCU;/thumb/f/f2/Stellaria_media_04.JPG/640px-Stellaria_media_04.JPG" prm="CC BY-SA     3.0"><ind>meza <tld/>o</ind> (Stellaria media), Riga, Latvujo
         <fnt>
           <aut>Sanja565658, propra verko</aut>
           <lok><url ref="&WCW;/File:Stellaria_media_04.JPG">&WCW;/File:Stellaria_media_04.JPG</url></lok>
         </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_k;&c_r;&c_y;&c_c;&c_a; &c_b;&c_je;&c_l;&c_a;&c_ja;</trd>
  <trd lng="ca">morr&oacute;</trd>
  <trd lng="de">Vogel-Sternmiere</trd>
  <trdgrp lng="en">
    <trd>Stellaria media</trd>,
    <trd>common chickweed</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&f_g;&a_n;&a_d;&a_m;&f_k; &a_r;&a_A;&f_y;&a_j;</trd>,
    <trd>&a_d;&a_A;&a_n;&a_h; &a_q;&a_n;&a_A;&a_r;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">mouron blanc</trd>
  <trd lng="hu">ty&uacute;kh&uacute;r</trd>
  <trd lng="la">Stellaria media</trd>
  <trd lng="nl">vogelmuur</trd>
  <trd lng="pl">gwiazdnica pospolita</trd>
  <trdgrp lng="zh">
    <trd>&#x7E41;&#x7E37; <pr>f&#xE1;nl&#x1D4;:</pr></trd>,
    <trd>&#x7E41;&#x7F15; <pr>f&#xE1;nl&#x1D4;:</pr></trd>
  </trdgrp>
</drv>
</art>
