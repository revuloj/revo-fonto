<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>stelari</rad>/o
</kap>

<drv mrk="stelari.0o">
  <kap><tld/>o</kap>
  <snc mrk="stelari.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super"
      cel="kariof1.0oj">kariofilacoj</ref>
      (<trd lng="la">Stellaria</trd>).
    </dif>
  </snc>
  <bld lok="../bld/stelari.jpg">
    <ind>granda <tld/>o</ind>
    <trd lng="de">Echte Sternmiere</trd>
    <trd lng="fa">&f_g;&a_n;&a_d;&a_m;&f_k; &a_z;&a_n;&f_g;&a_w;&a_l;&a_h;&zwnj;&a_A;&f_y;</trd>
    <trd lng="la">Stellaria holostea</trd>
    <trd lng="hu">olocs&aacute;ncsillagh&uacute;r</trd>
    <trd lng="nl">grote muur<klr tip="ind"> (plant)</klr></trd>
  </bld>
  <trd lng="be">&c_z;&c_o;&c_r;&c_k;&c_a;&c_w;&c_k;&c_a;</trd>
  <trd lng="de">Sternmiere</trd>
  <trd lng="fa">&f_g;&a_n;&a_d;&a_m;&f_k;</trd>
  <trd lng="hu">csillagh&uacute;r</trd>
  <trd lng="nl">muur<klr tip="ind"> (plant)</klr></trd>
  <trd lng="ru">&c_z;&c_v;&c_je;&c_z;&c_d;&c_ch;&c_a;&c_t;&c_k;&c_a;</trd>
</drv>

<drv mrk="stelari.meza0o">
  <kap>meza <tld/>o</kap>
  <snc mrk="stelari.meza0o.BOT">
  <uzo tip="fak">BOT</uzo>
    <dif>
      Specio de <ref tip="super" cel="stelari.0o.BOT">stelario</ref>
      malgranda, ku&scirc;a sur la grundo, kun nete dupartaj petaloj,
      komuna en kampoj kaj la&ubreve; vojoj de mezvarmaj landoj.
    </dif>
  <bld lok="../bld/stelari1.jpg">
  </bld>
  </snc>
  <trd lng="de">Vogel-Sternmiere</trd>
  <trdgrp lng="fa">
    <trd>&f_g;&a_n;&a_d;&a_m;&f_k; &a_r;&a_A;&f_y;&a_j;</trd>,
    <trd>&a_d;&a_A;&a_n;&a_h; &a_q;&a_n;&a_A;&a_r;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">mouron blanc</trd>
  <trd lng="hu">ty&uacute;kh&uacute;r</trd>
  <trd lng="la">Stellaria media</trd>
  <trd lng="nl">vogelmuur</trd>
</drv>
</art>
