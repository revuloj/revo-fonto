<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kilometr</rad>/o
</kap>
<drv mrk="kilome.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de longo, egala al
      mil <ref tip="prt" cel="metr.0o.unuo">metroj</ref>; simb. km:
      <ekz>
        tiu &ccirc;i rivero havas ducent <tld/>ojn da longo
        <fnt><bib>F</bib><lok>&FE; 32</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_l;&c_a;&c_m;&c_e;&c_t;&c_a;&c_r;</trd>
  <trd lng="de">Kilometer</trd>
  <trd lng="en">kilometer</trd>
  <trd lng="fr">kilom&egrave;tre</trd>
  <trd lng="hu">kilom&eacute;ter</trd>
  <trd lng="nl">kilometer</trd>
  <trd lng="ru">&c_k;&c_i;&c_l;&c_o;&c_m;&c_je;&c_t;&c_r;</trd>
</drv>
</art>
