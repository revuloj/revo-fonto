<art mrk="$Id: kilome.xml,v 1.23 2023/12/03 09:58:49 revo Exp $">
<kap>
  <ofc>*</ofc><ofc>1</ofc><ofc>7</ofc>
  <rad>kilometr</rad>/o
</kap>
<drv mrk="kilome.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de longo, egala
      al
      mil <ref tip="prt" cel="metr.0o.unuo">metroj</ref>; simb. km:
      <ekz>
        tiu &ccirc;i rivero havas ducent <tld/>ojn da longo
        <fnt><bib>F</bib><lok>&FE; 32</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
      <ref cel="hektom.0o">hektometro</ref>,
      <ref cel="dekame.0o">dekametro</ref>,
      <ref cel="metr.0o">metro</ref>,
      <ref cel="decime.0o">decimetro</ref>,
      <ref cel="centim1.0o">centimetro</ref>,
      <ref cel="milime.0o">milimetro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_l;&c_a;&c_m;&c_e;&c_t;&c_a;&c_r;</trd>
  <trd lng="bg">&c_k;&c_i;&c_l;&c_o;&c_m;&c_je;&c_t;&c_malmol;&c_r;</trd>
  <trdgrp lng="ca">
    <trd>quil&ograve;metre</trd>,
    <trd>kil&ograve;metre</trd>
  </trdgrp>
  <trd lng="cs">kilometr</trd>
  <trd lng="de">Kilometer</trd>
  <trd lng="en">kilometer</trd>
  <trd lng="es">kil&oacute;metro</trd>
  <trd lng="fa">&f_k;&f_y;&a_l;&a_w;&a_m;&a_t;&a_r;</trd>
  <trd lng="fr">kilom&egrave;tre</trd>
  <trd lng="hu">kilom&eacute;ter</trd>
  <trd lng="ja">&#x30AD;&#x30ED;&#x30E1;&#x30FC;&#x30C8;&#x30EB;  <pr>&#x304D;&#x308D;&#x3081;&#x30FC;&#x3068;&#x308B;</pr></trd>
  <trd lng="nl">kilometer</trd>
  <trd lng="pl">kilometr</trd>
  <trd lng="ro">kilometru</trd>
  <trd lng="ru">&c_k;&c_i;&c_l;&c_o;&c_m;&c_je;&c_t;&c_r;</trd>
  <trd lng="sk">kilometer</trd>
  <trdgrp lng="zh">
    <trd>&#x5343;&#x7C73; <pr>qi&#x101;nm&#x1D0;</pr></trd>,
    <trd>&#x7C81; <pr>qi&#x101;n</pr></trd>,
    <trd>&#x516C;&#x91CC; <pr>g&#x14D;ngl&#x1D0;</pr></trd>
  </trdgrp>
</drv>
</art>
