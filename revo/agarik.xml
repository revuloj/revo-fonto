<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>agarik</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="agarik.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="fung.lamen.0o">Lamenfungo</ref>.
    </dif>
  </snc>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="agarik.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Agaricus</trd>).
    </dif>
  </snc>
</drv>
<drv mrk="agarik.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de porfungoj (<trd lng="la">Agaricaceae</trd>),
      kies ombrelforma fruktokorpo prezentas de sube
      alcentrajn lamenojn.
    </dif>
    <ref tip="prt" cel="fung.lamen0o">lamenfungo</ref>
    <refgrp tip="sub">
      <ref cel="cxampinjon.0o">&ccirc;ampinjono</ref>,
      <ref cel="amanit.0o">amanito</ref>.
    </refgrp>
  </snc>
</drv>
</art>
