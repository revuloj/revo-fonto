<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>agarik</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="agarik.0o">
<kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Iu fungo el la familio <tld/>acoj.
    </dif>
  </snc>
  <trd lng="de">Bl&auml;tterpilz</trd>
  <trd lng="fr">agaric</trd>
</drv>
<drv mrk="agarik.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
       Familio de fungoj (<trd lng="la">Agaricaceae</trd>), 
       kun ombrelforma fruktokorpo kun lamenoj sube; = lamelfungo.
       Kelkaj specioj estas man&gcirc;eblaj, aliaj venenaj.  
     </dif>
     <refgrp tip="sub">
       <ref cel="cxampinjon.0o">&ccirc;ampinjono</ref>,
       <ref cel="amanit.o">amanito</ref>
     </refgrp>
  </snc>
  <trd lng="de">Bl&auml;tterpilze</trd>
</drv>
</art>
