<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: agarik.xml,v 1.32 2021/02/28 14:48:40 revo Exp $">
<kap>
  <rad>agarik</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="agarik.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="fung.lamen0o">Lamenfungo</ref>.
    </dif>
    <trd lng="de">Lamellenpilz</trd>
    <trd lng="hu">lemezesgomba</trd>
    <trd lng="ru">&c_p;&c_l;&c_a;&c_s;&c_t;&c_i;&c_n;&c_ch;&c_a;&c_t;&c_y;&c_j;
&c_g;&c_r;&c_i;&c_b;</trd>
  </snc>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio <ref tip="malprt" cel="agarik1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Agaricus</trd>); <ref tip="sin" cel="cxampinjon.0o">&ccirc;ampinjono</ref>:
      <ekz>
        la dungitoj laboris en 14 grandegaj fungejoj, kie necesis rikolti <tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <aut>LAST</aut>
          <vrk><url ref="https://www.monato.be/2003/007990.html">Sklaveco en fungejo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/a/ab/Kleiner_Waldchampignon_Agaricus_silvaticus.jpg/640px-Kleiner_Waldchampignon_Agaricus_silvaticus.jpg" prm="CC BY 3.0">Arbara <tld/>o (Agaricus silvaticus), &ccirc;e Ringingen, Germanujo
        <fnt>
          <aut>H. Krisp, propra verko</aut>
          <lok><url ref="&WCW;/File:Kleiner_Waldchampignon_Agaricus_silvaticus.jpg">https://commons.wikimedia.org/wiki/File:Kleiner_Waldchampignon_Agaricus_silvaticus.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="de">
      <trd>Egerling</trd>,
      <trd>Champignon</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>csiperke(gomba)</trd>,
      <trd>sampinyon(gomba)</trd></trdgrp>
    <trd lng="ru">&c_sh;&c_a;&c_m;&c_p;&c_i;&c_n;&c_mol;&c_o;&c_n;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>pe&ccaron;&aacute;rka</trd>,
    <trd>&zcaron;ampion</trd>
  </trdgrp>
  <trd lng="en">agaricus</trd>
  <trd lng="fr">agaric</trd>
  <trd lng="he">&#x5E1;&#x5D5;&#x5D2; &#x5E4;&#x5D8;&#x5E8;&#x5D9;&#x5D4;</trd>
  <trdgrp lng="it">
      <trd>agarico</trd>,
      <trd>champignon</trd>
    </trdgrp>
  <trd lng="nl">champignon</trd>
  <trd lng="pt">ag&aacute;rico</trd>
  <trdgrp lng="sk">
    <trd>&scaron;ampi&ncaron;&oacute;n</trd>,
    <trd>pe&ccaron;iarka</trd>,
    <trd>&scaron;ampi&ncaron;&oacute;n</trd>
  </trdgrp>
  <trd lng="sv">champinjon</trd>
  <trd lng="tr">&ccedil;ay&inodot;rmantar&inodot;</trd>
</drv>
</art>
