<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rekord.xml,v 1.32 2021/06/05 12:58:10 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>rekord</rad>/o
</kap>

<drv mrk="rekord.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">SPO</uzo>
    <dif>
      Plej supera oficiale konstatita supereco de konkursanto en sporto:
      <ekz>
        posedi la <tld/>on;
      </ekz>
      <ekz>
        starigi, superi, venki<fnt>B</fnt>,
      </ekz>
      <ekz>
        rompi <tld/>on.
      </ekz>
    </dif>
  </snc>
  <trd lng="af">rekord</trd>
  <trd lng="am">&#x1218;&#x12DD;&#x1308;&#x1265;</trd>
  <trd lng="ar">&#x633;&#x62C;&#x644;</trd>
  <trd lng="az">rekord</trd>
  <trd lng="be">&c_r;&c_e;&c_k;&c_o;&c_r;&c_d;</trd>
  <trd lng="bg">&c_r;&c_je;&c_k;&c_o;&c_r;&c_d;</trd>
  <trd lng="bn">&#x9B0;&#x9C7;&#x995;&#x9B0;&#x9CD;&#x9A1;</trd>
  <trd lng="cs">rekord</trd>
  <trd lng="da">rekord</trd>
  <trd lng="de">Rekord</trd>
  <trd lng="en">record <klr>(sport)</klr></trd>
  <trdgrp lng="es">
    <trd>marca</trd>,
    <trd>r&eacute;cord</trd>
  </trdgrp>
  <trd lng="fr">record</trd>
  <trdgrp lng="hu">
    <trd>cs&uacute;cs</trd>,
    <trd>rekord</trd>
  </trdgrp>
  <trd lng="hy">&#x563;&#x580;&#x561;&#x57C;&#x578;&#x582;&#x574;&#x568;</trd>
  <trd lng="nl">record</trd>
  <trd lng="pt">recorde</trd>
  <trd lng="ru">&c_r;&c_je;&c_k;&c_o;&c_r;&c_d;</trd>
  <trd lng="sk">rekord</trd>
  <trd lng="sq">rekord</trd>
  <trd lng="sv">rekord</trd>
  <trd lng="tl">talaan</trd>
  <trd lng="ur">&#x631;&#x6CC;&#x6A9;&#x627;&#x631;&#x688;</trd>
  <trd lng="vi">ghi</trd>
</drv>

<drv mrk="rekord.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
  <uzo tip="fak">SPO</uzo>
    <dif>
      Tiu, kiu atingis kaj konservas <tld/>on.
    </dif>
    <refgrp tip="vid">
      <ref cel="cxampi.0o.SPO">&ccirc;ampiono</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_k;&c_a;&c_r;&c_d;&c_s;&c_m;&c_e;&c_n;</trd>
  <trd lng="bg">&c_r;&c_je;&c_k;&c_o;&c_r;&c_d;&c_mol;&c_o;&c_r;</trd>
  <trd lng="cs">rekordman</trd>
  <trd lng="de">Rekordhalter</trd>
  <trdgrp lng="en">
    <trd>record holder</trd>,
    <trd>record breaker</trd>,
    <trd>record achiever</trd>
  </trdgrp>
  <trd lng="es">poseedor del r&eacute;cord</trd>
  <trd lng="fr">recordman</trd>
  <trdgrp lng="hu">
    <trd>cs&uacute;cstart&oacute;</trd>,
    <trd>rekorder</trd>
  </trdgrp>
  <trd lng="nl">recordhouder</trd>
  <trd lng="pt">recordista</trd>
  <trd lng="ru">&c_r;&c_je;&c_k;&c_o;&c_r;&c_d;&c_s;&c_m;&c_je;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>rekordman</trd>,
    <trd>rekord&eacute;r</trd>
  </trdgrp>
  <trd lng="sv">rekordh&aring;llare</trd>
</drv>
</art>
