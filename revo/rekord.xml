<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>rekord</rad>/o
</kap>
<drv mrk="rekord.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      Plej supera oficiale konstatita supereco de konkursanto en sporto:
      <ekz>
        posedi la <tld/>on;
      </ekz>
      <ekz>
        starigi, superi, venki<fnt>B</fnt>,
      </ekz>
      <ekz>
        rompi <tld/>on.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Rekord</trd>
  <trd lng="nl">record</trd>
  <trd lng="ru">&c_r;&c_je;&c_k;&c_o;&c_r;&c_d;</trd>
  <trd lng="sv">rekord</trd>
  <trdgrp lng="es">
    <trd>marca</trd>,
    <trd>record</trd>
  </trdgrp>
</drv>

<drv mrk="rekord.0ulo">
  <kap><tld/>ulo</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      Tiu, kiu atingis kaj konservas <tld/>on
    </dif>
    <refgrp tip="sin">
      <ref cel="cxampi.0o">&ccirc;ampiono</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Rekordhalter</trd>
  <trd lng="nl">recordhouder</trd>
  <trd lng="ru">&c_r;&c_je;&c_k;&c_o;&c_r;&c_d;&c_s;&c_m;&c_je;&c_n;</trd>
  <trd lng="sv">rekordh&aring;llare</trd>
</drv>
</art>
