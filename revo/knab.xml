<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>knab</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="knab.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="knab.0o.KOMUNE">
    <dif>
      Virseksa infano:
      <ekz>
        mi vokas la <tld/>on, kaj li venas
        <fnt><bib>F</bib>, <lok>&FE; 16</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_l;&c_o;&c_p;&c_ch;&c_y;&c_k;</trd>
  <trd lng="br">paotr</trd>
  <trd lng="cs">chlapec</trd>
  <trdgrp lng="de">
    <trd>Junge</trd>,
    <trd>Knabe</trd>,
    <trd>Bub</trd>
  </trdgrp>
  <trd lng="en">boy</trd>
  <trd lng="es">chico</trd>
  <trdgrp lng="fr">
    <trd>gar&ccedil;on<klr tip="ind"> (enfant m&acirc;le, jeune
homme)</klr></trd>,
    <trd>jeune <ind>homme</ind></trd>,
    <trd>gars</trd>
  </trdgrp>
  <trd lng="id"><ind>anak</ind> laki-laki</trd>
  <trd lng="it">ragazzo</trd>
  <trd lng="nl">jongen</trd>
  <trdgrp lng="pl">
    <trd>ch&lstroke;opiec</trd>,
    <trd>ch&lstroke;opak</trd>,
    <trd>ch&lstroke;opczyna</trd>,
    <trd>ch&lstroke;opi&eogonek;<klr> (staromodne)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_a;&c_l;&c_mol;&c_ch;&c_i;&c_k;</trd>
  <trd lng="sk">chlapec</trd>
  <trd lng="sv">pojke</trd>
  <trdgrp lng="tr">
    <trd>erkek &ccedil;ocuk</trd>,
    <trd>o&gbreve;lan</trd>,
    <trd>delikanl&inodot;</trd>,
    <trd>gen&ccedil; erkek</trd>
  </trdgrp>
</drv>

<drv mrk="knab.0ino">
  <kap><ofc>*</ofc><tld/>ino</kap>
  <snc mrk="knab.0ino.KOMUNE">
    <dif>
      Inseksa infano:
      <ekz>
        Izabela scias, kiu estas la <tld/>ino en la spegulo
        <fnt><bib>Lasu</bib>, <lok>1 &leftquot;Izabela&rightquot;</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_ja;&c_w;&c_ch;&c_y;&c_n;&c_k;&c_a;</trd>
  <trd lng="br">plac'h</trd>
  <trd lng="de">M&auml;dchen</trd>
  <trd lng="en">girl</trd>
  <trd lng="es">chica</trd>
  <trdgrp lng="fr">
    <trd>fille<klr tip="ind"> (enfant femelle, jeune femme)</klr></trd>,
    <trd>jeune <ind>femme</ind></trd>
  </trdgrp>
  <trd lng="id"><ind>anak</ind> perempuan</trd>
  <trd lng="it">ragazza</trd>
  <trd lng="nl">meisje</trd>
  <trdgrp lng="pl">
    <trd>dziewczyna</trd>,
    <trd>dziewcz&eogonek;<klr> (staromodne)</klr></trd>,
    <trd>dziewuszka<klr> (staromodne)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_je;&c_v;&c_o;&c_ch;&c_k;&c_a;</trd>
  <trd lng="sk">dievƒça</trd>
  <trd lng="sv">flicka</trd>
  <trdgrp lng="tr">
    <trd>k&inodot;z &ccedil;ocuk</trd>,
    <trd>gen&ccedil; k&inodot;z</trd>
  </trdgrp>
</drv>

<drv mrk="knab.kuir0o">
  <kap>kuir<tld/>o</kap>
  <snc mrk="knab.kuir0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Juna helpanto en restoracia kuirejo:
      <ekz>
        kuir<tld/>oj, grimacante kaj interbatante &ccirc;e la kuirforno,
        sukcesadis samtempe a&ubreve;skulti la konversacion
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/mondo.html">
          La mondo brilanta</url></vrk>,
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">marmiton</trd>
</drv>

<drv mrk="knab.stal0o">
  <kap>stal<tld/>o</kap>
  <snc mrk="knab.stal0o.RAJD">
    <dif>
      Juna helpanto en prizorgado de stalo,
      de tieaj bestoj:
      <ekz>
        &leftquot;mi lasos vin vespere senpage en la cirkon&rightquot;
        diris la hirta stal<tld/>o
        <fnt>
          <aut>S. Bubeni&ccaron;</aut>,
          <vrk>Sub Cirkotendo</vrk>,
          <lok>1969</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">gar&ccedil;on d'&eacute;curie</trd>
</drv>
</art>
