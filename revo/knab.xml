<drv mrk="knab.0ego">
  <kap>
    <tld/>ego
  </kap>
  <snc mrk="knab.0ego.KOMUNE">
    <dif>
      Juna, fortika, senmalica viro:
      <ekz>
        li kisis sian patrinon tiel skuege, ke &scirc;i preska&ubreve; renversi&gcirc;is, li efektive estis
        sova&gcirc;a <tld/>ego
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>!
      </ekz>
      <ekz>
        oni metu min anta&ubreve; anaron da <tld/>egoj kiel mi,
        tiam Germanujo fari&gcirc;os respubliko, kontra&ubreve; kiu
        Romo kaj Sparto estos mona&hcirc;inejoj
        <fnt>
          <aut>F. Schiller, trad. L. L. Zamenhof</aut>
          <vrk>La Rabistoj</vrk>
          <lok>1928</lok>
        </fnt>!
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="brav.0ulo">bravulo</ref>,
    </refgrp>
    <trdgrp lng="fr">
      <trd>brave gars</trd>,
      <trd>gaillard</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#30007;&#23376;&#27721; <pr>n&aacute;nz&#464;h&agrave;n</pr></trd>,
    <trd>&#22766;&#22763; <pr>zhu&agrave;ngsh&igrave;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_h;&c_l;&c_a;&c_p;&c_ch;&c_y;&c_n;&c_a;</trd>
  <trd lng="de">Bursche</trd>
  <trd lng="ja">&#12356;&#12383;&#12378;&#12425;&#22909;&#12365;&#12398;&#30007;
  <pr>&#12356;&#12383;&#12378;&#12425;&#12377;&#12365;&#12398;&#12362;&#12392;&#12371;</pr></trd>
  <trdgrp lng="uk">
    <trd>&c_h;&c_l;&c_o;&c_p;&c_c;&c_ju;&c_g;&c_a;</trd>,
    <trd>&c_h;&c_l;&c_o;&c_p;&#8217;&c_ja;&c_g;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_u;&c_b;&#8217;&c_ja;&c_g;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_u;&c_b;&c_ch;&c_i;&c_s;&c_mol;&c_k;&c_o;</trd>
