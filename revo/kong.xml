<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kong</rad>/o
</kap>

<drv mrk="kong.0o.brazavila">
  <kap>
    <tld lit="K"/>o Brazavila, Okcidenta <tld lit="K"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="kong.0o.brazavila.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la centra <ref tip="malprt" cel="afrik.A0o">Afriko</ref> 
      kun &ccirc;efurbo Brazavilo.
      Teritorio de Respubliko Kongo.
      ISO-3166 kodo: CG.
      &Gcirc;i limas kun
      <ref tip="vid" cel="angol.0o.GEOG">Angolo</ref>,
      <ref tip="vid" cel="afrik.centr0o.GEOG">Centr-Afriko</ref>,
      <ref tip="vid" cel="gabon.0o.GEOG">Gabono</ref>,
      <ref tip="vid" cel="kamerun.0o.GEOG">Kameruno</ref>,
      <ref tip="vid" cel="kong.0o.kinsxasa.GEOG">Kongo
      Kin&scirc;asa</ref>
      kaj <ref tip="vid" cel="atlant.0o.GEOG">Atlantiko</ref>.
    </dif>
    <trd lng="bg">&c_R;&c_je;&c_p;&c_u;&c_b;&c_l;&c_i;&c_k;&c_a; <ind>&c_K;&c_o;&c_n;&c_g;&c_o;</ind></trd>
    <trd lng="de">Republik <ind>Kongo</ind></trd>
    <trd lng="en">Republic of the <ind>Congo</ind></trd>
    <trdgrp lng="pl">
      <trd>Kongo</trd>,
      <trd>Republika <ind>Konga</ind></trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="kong.0o.kinsxasa">
  <kap>
    <tld lit="K"/>o Kin&scirc;asa, Orienta <tld lit="K"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="kong.0o.kinsxasa.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la centra <ref tip="malprt" cel="afrik.A0o">Afriko</ref> 
      kun &ccirc;efurbo Kin&scirc;aso.
      Teritorio de Demokratia Respubliko Kongo.
      &Gcirc;is 1908 nomata Libera Kongo, poste - Belga Kongo. Inter
      1971 kaj 
      1997 nomata Zairo, dum mallonga tempo anka&ubreve;
      Kongo-L&eacute;opoldville.
      ISO-3166 kodo: CD.
      &Gcirc;i limas kun
      <ref tip="vid" cel="angol.0o.GEOG">Angolo</ref>,
      <ref tip="vid" cel="burund.0o.GEOG">Burundo</ref>,
      <ref tip="vid" cel="afrik.centr0o.GEOG">Centr-Afriko</ref>,
      <ref tip="vid" cel="kong.0o.brazavila.GEOG">Kongo Brazavila</ref>,
      <ref tip="vid" cel="ruand.0o.GEOG">Ruando</ref>,
      <ref tip="vid" cel="sudan.0o.GEOG">Sudano</ref>,
      <ref tip="vid" cel="tanzani.0o.GEOG">Tanzanio</ref>,
      <ref tip="vid" cel="ugand.0o.GEOG">Ugando</ref>,
      <ref tip="vid" cel="zambi.0o.GEOG">Zambio</ref>
      kaj <ref tip="vid" cel="atlant.0o.GEOG">Atlantiko</ref>.
    </dif>
    <trd lng="bg">&c_D;&c_je;&c_m;&c_o;&c_k;&c_r;&c_a;&c_t;&c_i;&c_ch;&c_n;&c_a; &c_r;&c_je;&c_p;&c_u;&c_b;&c_l;&c_i;&c_k;&c_a;
    <ind>&c_K;&c_o;&c_n;&c_g;&c_o;</ind></trd>
    <trd lng="de">Demokratische Republik <ind>Kongo</ind></trd>
    <trd lng="en">Democratic Republic of the <ind>Congo</ind></trd>
    <trdgrp lng="pl">
      <trd>Demokratyczna Republika <ind>Kongo</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

