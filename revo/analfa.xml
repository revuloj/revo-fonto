<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: analfa.xml,v 1.32 2023/10/28 08:33:06 revo Exp $">
<kap>
  <rad>analfabet</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="analfa.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Homo, kiu scipovas nek legi nek skribi:
      <ekz>
        li lernis skribi sian nomon, sed cetere li estas <tld/>o
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la Esperanta lingvokolektivo la gamo etendi&gcirc;as de elementlerneja edukiteco
        &gcirc;is la suproj de akademia klereco <klr>[&#x2026;]</klr> feli&ccirc;e, tamen, forestas
        <tld/>oj
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi estas <tld/>o <klr>(<ref cel="laik.0o.FIG">laiko</ref>)</klr> pri medicino
        <fnt>
          <bib>Monato</bib>
          <aut>Lin Sheng</aut>
          <vrk><url ref="https://www.monato.be/2006/008670.php">Forpasis mia karulino</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        li konkludis, ke la uzvortaro de <tld/>oj kaj duon<tld/>oj ne transpasas 500
        vortojn
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        televidaj prezentistoj, kiuj estas, a&ubreve; &scirc;ajnas esti,
        <tld/>oj kaj malkleruloj
        <fnt><aut>William Auld</aut><vrk>Dekadenco de
        lingvo</vrk><bib>Monato</bib>
        <lok>jaro 1996a, numero 2a, p. 21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_p;&c_ib;&c_s;&c_mol;&c_m;&c_je;&c_n;&c_n;&c_y; <klr>(&c_n;&c_a;&c_z;.)</klr></trd>
  <trd lng="ca">analfabet</trd>
  <trdgrp lng="cs">
    <trd>analfabet</trd>,
    <trd>negramotn&yacute; &ccaron;lov&ecaron;k</trd>
  </trdgrp>
  <trd lng="de">Analphabet</trd>
  <trd lng="en">illiterate</trd>
  <trd lng="es">analfabeto</trd>
  <trd lng="fr">analphab&egrave;te<klr> (subst.)</klr></trd>
  <trd lng="he">&bet;&#x5BC;&vav;&#x5BC;&resh;</trd>
  <trdgrp lng="hu">
    <trd>&iacute;r&aacute;studatlan</trd>,
    <trd>analfab&eacute;ta</trd>
  </trdgrp>
  <trd lng="it">analfabeta</trd>
  <trdgrp lng="ja">
    <trd>&#x8AAD;&#x307F;&#x66F8;&#x304D;&#x306E;&#x3067;&#x304D;&#x306A;&#x3044;&#x4EBA;  <pr>&#x3088;&#x307F;&#x304B;&#x304D;&#x306E;&#x3067;&#x304D;&#x306A;&#x3044;&#x3058;&#x3093;</pr></trd>,
    <trd>&#x975E;&#x8B58;&#x5B57;&#x8005;  <pr>&#x3072;&#x3057;&#x304D;&#x3058;&#x3057;&#x3083;</pr></trd>
  </trdgrp>
  <trd lng="nl">analfabeet</trd>
  <trd lng="pl">analfabeta</trd>
  <trd lng="pt">analfabeto</trd>
  <trd lng="ru">&c_n;&c_je;&c_g;&c_r;&c_a;&c_m;&c_o;&c_t;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>negramotn&yacute;</trd>,
    <trd>analfabet</trd>
  </trdgrp>
  <trd lng="sv">analfabet</trd>
  <trd lng="uk">&c_n;&c_je;&c_p;&c_i;&c_s;&c_mol;&c_m;&c_je;&c_n;&c_n;&c_i;&c_j;&nbsp;(&c_ib;&c_m;.)</trd>
</drv>

<drv mrk="analfa.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Kapabla nek legi nek skribi:
      <ekz>
        li kaj lia edzino estis <tld/>aj
        <fnt>
          <bib>KrM</bib>
          <lok>31. Ricevis Bandito La&ubreve; Sia Merito.</lok>
        </fnt>;
      </ekz>
      <ekz>
        mankas &ccirc;inaj lernejoj, kie la lernejanoj lernas &ccirc;iun studobjekton en la &ccirc;ina, 
        kaj studas la anglan kaj la malajan lingvojn <klr>[&#x2026;]</klr>,
        &ccirc;inoj kaj hindoj scipovas paroli sian gepatran lingvon sed estas <tld/>aj en tiu
        lingvo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9402-malajzio.txt">Malajzio</url></vrk>
          <lok>94/02</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="leg.ne0opova">nelegopova</ref>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_p;&c_ib;&c_s;&c_mol;&c_m;&c_je;&c_n;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>analfabetsk&yacute;</trd>,
    <trd>negramotn&yacute;</trd>
  </trdgrp>
  <trd lng="fr">analphab&egrave;te<klr> (adj.)</klr></trd>
  <trd lng="pl">analfabetyczny</trd>
  <trd lng="sk">negramotn&yacute;</trd>
</drv>


<drv mrk="analfa.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Nekapablo legi kaj skribi.
    </dif>
  </snc>
  <snc mrk="analfa.0eco.gxenerala">
    <dif>
      &Gcirc;enerala manko de leg- kaj skribkapablo en socio,
      <ref tip="dif" cel="analfa.0ismo">analfabetismo</ref>:
      <ekz>
        For <tld lit="A"/>econ
        <fnt>
          <bib>Metrop</bib>
        </fnt>!
      </ekz>
      <ekz>
        malsato kaj <tld/>eco bremsas socian progreson
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>eco inter virinoj kvaroble pli altas ol &ccirc;e viroj
        <fnt>
          <aut>Bardhyl Selimi</aut><vrk>Kosovo: Novaj direktoj al la
          estonto</vrk>
          <bib>Monato</bib><lok>jaro 2003a, numero 12a, p.
          10a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="leg.0opovo">legopovo</ref>
    <trdgrp lng="zh">
    <trd>&#x8B58;&#x5B57;&#x7387; <pr>sh&#xED;z&#xEC;shu&#xE0;i</pr></trd>,
    <trd>&#x8BC6;&#x5B57;&#x7387; <pr>sh&#xED;z&#xEC;shu&#xE0;i</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_n;&c_je;&c_p;&c_ib;&c_s;&c_mol;&c_m;&c_je;&c_n;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="ca">analfabetisme</trd>
  <trd lng="cs">analfabetstv&iacute;</trd>
  <trd lng="de">Analphabetismus</trd>
  <trd lng="en">illiteracy</trd>
  <trd lng="fr">analphab&eacute;tisme</trd>
  <trdgrp lng="hu">
    <trd>&iacute;r&aacute;studatlans&aacute;g</trd>,
    <trd>analfabetizmus</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8AAD;&#x307F;&#x66F8;&#x304D;&#x306E;&#x3067;&#x304D;&#x306A;&#x3044;&#x3053;&#x3068;  <pr>&#x3088;&#x307F;&#x304B;&#x304D;&#x306E;&#x3067;&#x304D;&#x306A;&#x3044;&#x3053;&#x3068;</pr></trd>,
    <trd>&#x975E;&#x8B58;&#x5B57;  <pr>&#x3072;&#x3057;&#x304D;&#x3058;</pr></trd>
  </trdgrp>
  <trd lng="pl">analfabetyzm</trd>
  <trd lng="pt">analfabetismo</trd>
  <trd lng="ru">&c_n;&c_je;&c_g;&c_r;&c_a;&c_m;&c_o;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">analfabetizmus</trd>
  <trd lng="sv">analfabetism</trd>
</drv>

<drv mrk="analfa.0ismo">
  <kap>
    <tld/>ismo
  </kap>
  <snc>
    <dif>
      &Gcirc;eneralaj sociaj kondi&ccirc;oj, kiu malfavoras edukadon al bazaj scioj de legado kaj
      skribado:
      <ekz>
        lukto kontra&ubreve; <tld/>ismo
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        aliaj problemoj, kiel <klr>[&#x2026;]</klr> <tld/>ismo, pezegas sur la situacio
        ekonomia de la malagasa kamparo
        <fnt>
          <bib>Monato</bib>
          <aut>Fidilalao Henriel</aut>
          <vrk><url ref="https://www.monato.be/2006/008754.php">Rizo: senelira problemo</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_p;&c_ib;&c_s;&c_mol;&c_m;&c_je;&c_n;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="ca">analfabetisme</trd>
  <trd lng="cs">analfabetstv&iacute;</trd>
  <trd lng="de">Analphabetismus</trd>
  <trd lng="en">illiteracy</trd>
  <trd lng="fr">analphab&eacute;tisme</trd>
  <trdgrp lng="hu">
    <trd>&iacute;r&aacute;studatlans&aacute;g</trd>,
    <trd>analfabetizmus</trd>
  </trdgrp>
  <trd lng="pl">analfabetyzm</trd>
  <trd lng="pt">analfabetismo</trd>
  <trd lng="ru">&c_n;&c_je;&c_g;&c_r;&c_a;&c_m;&c_o;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">analfabetizmus</trd>
  <trd lng="sv">analfabetism</trd>
</drv>


</art>
