<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: miligr.xml,v 1.19 2022/03/23 14:01:46 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>miligram</rad>/o
</kap>
<drv mrk="miligr.0o">
  <kap><tld/>o</kap>
  <mlg kod="SI">mg</mlg>
  <snc mrk="miligr.0o.mezurunuo">
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de maso, milono
      de <ref tip="malprt" cel="gram.0o">gramo</ref>:
      <ekz>
        homon ili pesis sur analiza pesilo &gcirc;is la milona parto de la <tld/>o
        <fnt>
          <aut>S&aacute;ndor Szathm&aacute;ri</aut>
          <vrk><url ref="https://tekstaro.com/t?nomo=vojagho-al-kazohinio">Voja&gcirc;o al Kazohinio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ekde 1907, la metra karato de 200 <tld/>oj estis internacie adoptita
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=36564">Karato</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="mili1.0">mili-</ref>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_l;&c_ib;&c_g;&c_r;&c_a;&c_m;</trd>
  <trd lng="ca">mil&middot;ligram</trd>
  <trd lng="cs">miligram</trd>
  <trd lng="de">Milligramm</trd>
  <trd lng="en">milligram</trd>
  <trd lng="es">miligramo</trd>
  <trd lng="fr">milligramme</trd>
  <trd lng="hu">milligramm</trd>
  <trd lng="nl">milligram</trd>
  <trd lng="pl">miligram</trd>
  <trd lng="pt">miligrama</trd>
  <trd lng="ru">&c_m;&c_i;&c_l;&c_l;&c_i;&c_g;&c_r;&c_a;&c_m;&c_m;</trd>
  <trd lng="sk">miligram</trd>
  <trd lng="uk">&#x43C;&#x456;&#x43B;&#x456;&#x433;&#x440;&#x430;&#x43C;</trd>
</drv>
</art>
