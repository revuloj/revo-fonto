  <rad>kontest</rad>/i <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="kontes.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kontes.0i.KOMUNE">
    <dif>
      Diskute malakcepti aserton a&ubreve; postulon:
      <ekz>
        nuntempaj sociaj luktoj neniel celas <tld/>i la sistemon sed
        defendi interesojn apartajn, limigitajn
        <fnt>
         <aut>D. Kessous</aut>,
         <vrk><url ref="http://www.satesperanto.org/Klasbatalo-vorto-ideo-koncepto.html">
         Klasbatalo : vorto, ideo, koncepto?</url></vrk>,
         <lok>[vidita en 2015]</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konsen.mal0i">malkonsenti</ref>,
      <ref cel="rifuz.0i">rifuzi</ref>.
    </refgrp>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>diskute malakcepti</ke>, <ke>rifuzi</ke>, <ke>malkonsenti</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_a;&c_s;&c_p;&c_r;&c_e;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_v;&c_ja;&c_r;&c_g;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_ja;&c_r;&c_e;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_k;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol;
    <ind>&c_n;&c_ja;&c_z;&c_g;&c_o;&c_d;&c_u;</ind></trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_o;&c_s;&c_p;&c_o;&c_r;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_v;&c_malmol;&c_z;&c_r;&c_a;&c_z;&c_ja;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pop&iacute;rat</trd>,
    <trd>pop&rcaron;&iacute;t</trd>,
    <trd>vyvracet</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>abstreiten</trd>,
     <trd>bestreiten</trd>,
     <trd>leugnen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>call into <ind>question</ind></trd>,
    <trd>contest</trd>,
    <trd>dispute <klr>(the truth of)</klr></trd>,
    <trd><ind>deny</ind> the truth of</trd>,
    <trd>gainsay</trd>,
    <trd>refuse to <ind>accept</ind></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>cuestionar</trd>,
    <trd>contradecir</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_z;&f_y;&a_r; &a_s;&a_w;&a_A_madda;&a_l; &a_b;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_r;&a_d; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">contester</trd>
  <trd lng="ja">&#30064;&#35696;&#12434;&#21809;&#12360;&#12427;  <pr>&#12356;&#12366;&#12434;&#12392;&#12394;&#12360;&#12427;</pr></trd>
  <trd lng="nl">betwisten</trd>
  <trdgrp lng="pl">
    <trd>kwestionowa&cacute;</trd>,
    <trd>podwa&zdot;a&cacute;<klr> (czyje&sacute; racje)</klr></trd>,
    <trd>odrzuca&cacute;<klr> (przyj&eogonek;cie
    czego&sacute;)</klr></trd>,
    <trd>wyra&zdot;a&cacute; <ind>sprzeciw</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>contestar</trd>,
    <trd>impugnar</trd>,
    <trd>refutar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_s;&c_p;&c_a;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_o;&c_z;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol;
    &c_n;&c_je;&c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>popiera&tcaron;</trd>,
    <trd>poprie&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#34987;&#20105;&#35770; <pr>b&egrave;izh&emacron;ngl&ugrave;n</pr></trd>,
    <trd>&#34987;&#30424;&#38382; <pr>b&egrave;ip&aacute;nw&egrave;n</pr></trd>,
    <trd>&#36777;&#39539; <pr>bi&agrave;nb&oacute;</pr></trd>,
    <trd>&#29409;&#36084; <pr>ji&#462;ol&agrave;i</pr></trd>,
    <trd>&#25269;&#36084; <pr>d&#464;l&agrave;i</pr></trd>,
    <trd>&#25273;&#26432; <pr>m&#466;sh&amacron;</pr></trd>,
    <trd>&#36777;&#22068; <pr>bi&agrave;nzu&#464;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kontes.ne0ebla">
  <kap>ne<tld/>ebla</kap>
  <snc mrk="kontes.ne0ebla.KOMUNE">
    <dif>
      Kiun oni ne povas <tld/>i, &ccirc;ar &gcirc;i
      evidentas, trudi&gcirc;as...:
      <ekz>
        <ind>ne<tld/>ebla pruvo</ind>.
        <trd lng="be">&c_n;&c_je;&c_a;&c_b;&c_v;&c_je;&c_r;&c_zh;&c_n;&c_y;
        <ind>&c_d;&c_o;&c_k;&c_a;&c_z;</ind></trd>
        <trd lng="nl">onweerlegbaar bewijs</trd>
        <trd lng="pl">niepodwa&zdot;alny dow&oacute;d</trd>
        <trdgrp lng="ru">
          <trd>&c_n;&c_je;&c_o;&c_s;&c_p;&c_o;&c_r;&c_i;&c_m;&c_o;&c_je;
          &c_d;&c_o;&c_k;&c_a;&c_z;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>,
          <trd>&c_n;&c_je;&c_o;&c_p;&c_r;&c_o;&c_v;&c_je;&c_r;&c_zh;&c_i;&c_m;&c_o;&c_je;
          &c_d;&c_o;&c_k;&c_a;&c_z;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_a;&c_s;&c_p;&c_r;&c_e;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_ja;&c_s;&c_s;&c_p;&c_r;&c_e;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_a;&c_b;&c_v;&c_ja;&c_r;&c_g;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_a;&c_b;&c_v;&c_je;&c_r;&c_zh;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_n;&c_je;&c_o;&c_s;&c_p;&c_o;&c_r;&c_i;&c_m;&c_o;</trd>
  <trd lng="cs">nepopirateln&yacute;</trd>
  <trdgrp lng="de">
    <trd>unbestreitbar</trd>,
    <trd>unleugbar</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>incontestable</trd>,
    <trd>unquestionable</trd>,
    <trd>indisputable</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>incuestionable</trd>,
    <trd>indiscutible</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_m;&a_s;&a_l;&a_shadda;&a_m;</trd>,
    <trd>&a_b;&f_y; &f_ch;&a_w;&a_n; &a_w; &f_ch;&a_r;&a_A;</trd>
  </trdgrp>
  <trd lng="fr">incontestable</trd>
  <trdgrp lng="ja">
    <trd>&#30064;&#35542;&#12398;&#20313;&#22320;&#12398;&#12394;&#12356;  <pr>&#12356;&#12429;&#12435;&#12398;&#12424;&#12385;&#12398;&#12394;&#12356;</pr></trd>,
    <trd>&#21205;&#12363;&#12375;&#38627;&#12356;  <pr>&#12358;&#12372;&#12363;&#12375;&#12364;&#12383;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">onweerlegbaar</trd>
  <trdgrp lng="sk">
    <trd>neod&scaron;kriepite&lcaron;n&yacute;</trd>,
    <trd>nepopierate&lcaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#19981;&#21487;&#21542;&#35469; <pr>b&ugrave;k&ecaron;f&#466;ur&egrave;n</pr></trd>,
    <trd>&#19981;&#23481;&#20105;&#36777; <pr>b&ugrave;r&oacute;ngzh&emacron;ngbi&agrave;n</pr></trd>,
    <trd>&#19981;&#23481;&#32622;&#30097; <pr>b&ugrave;r&oacute;ngzh&igrave;y&iacute;</pr></trd>,
    <trd>&#19981;&#33021;&#21542;&#35469; <pr>b&ugrave;n&eacute;ngf&#466;ur&egrave;n</pr></trd>,
    <trd>&#26126;&#25670;&#30528; <pr>m&iacute;ngb&#462;izhe</pr></trd>,
    <trd>&#26080;&#21487;&#35763;&#35328; <pr>w&uacute;k&ecaron;hu&igrave;y&aacute;n</pr></trd>,
    <trd>&#30830;&#20991; <pr>qu&egrave;z&aacute;o</pr></trd>,
    <trd>&#26080;&#21487;&#36777;&#39539; <pr>w&uacute;k&ecaron;bi&agrave;nb&oacute;</pr></trd>,
    <trd>&#39072;&#25169;&#19981;&#30772; <pr>di&amacron;np&umacron;b&ugrave;p&ograve;</pr></trd>
  </trdgrp>
</drv>
