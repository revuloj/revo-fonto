<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>kontest</rad>/i
</kap>
<drv mrk="kontes.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Diskute malakcepti aserton a&ubreve; postulon:
      <ekz>
        <ind>ne<tld/>ebla pruvo</ind>.
        <trd lng="nl">onweerlegbaar bewijs</trd>
        <trdgrp lng="ru">
          <trd>&c_n;&c_je;&c_o;&c_s;&c_p;&c_o;&c_r;&c_i;&c_m;&c_o;&c_je; &c_d;&c_o;&c_k;&c_a;&c_z;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>,
          <trd>&c_n;&c_je;&c_o;&c_p;&c_r;&c_o;&c_v;&c_je;&c_r;&c_zh;&c_i;&c_m;&c_o;&c_je; &c_d;&c_o;&c_k;&c_a;&c_z;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>
        </trdgrp>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="konsen.mal0i">malkonsenti</ref>,
      <ref cel="rifuz.0i">rifuzi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="en">
    <trd>call into question</trd>,
    <trd>contest</trd>,
    <trd>debate <klr>(the truth of)</klr></trd>,
    <trd>deny the truth of</trd>,
    <trd>gainsay</trd>,
    <trd>refuse to accept</trd>
  </trdgrp>
  <trd lng="nl">betwisten</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_s;&c_p;&c_a;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_o;&c_z;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol; &c_n;&c_je;&c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
