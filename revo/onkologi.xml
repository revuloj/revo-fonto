<art mrk="$Id: onkologi.xml,v 1.6 2019/10/16 17:10:27 revo Exp $">
<kap>
  <rad>onkologi</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="onkologi.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="onkologi.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="dif" cel="kancerologi.0o.MED">Kancerologio</ref>:
      <ekz>
        la&ubreve; la direktoro de la varsovia <tld lit="O"/>a Centro, profesoro 
        <nom>Marek Nowacki</nom>, medicina progreso estas grandega
        <fnt>
          <aut>Pola Radio</aut>,
          <vrk><url ref="http://www.polskieradio.pl/eo/dokument.aspx?iid=101862">
          Ni volas venki kanceron</url></vrk>,
          <lok>2009-02-06</lok>
        </fnt>;
      </ekz>
      <ekz>
        post peza <tld/>a malsano
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="https://www.monato.be/2005/008559.php"
              >Pri la aferoj misteraj</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">oncologia</trd>
  <trd lng="cs">onkologie</trd>
  <trd lng="de">Onkologie</trd>
  <trd lng="fr">oncologie</trd>
  <trd lng="sk">onkol&oacute;gia</trd>
</drv>
</art>

