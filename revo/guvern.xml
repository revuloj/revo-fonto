<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>guvern</rad>/i
</kap>
<drv mrk="guvern.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Direkti kaj prizorgi la edukadon de infano, kunvivante kun &gcirc;i.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>erziehen</trd>,
    <trd>beaufsichtigen</trd>
  </trdgrp>
  <trd lng="en">tutor</trd>
  <trd lng="fr">&eacute;duquer<klr tip="ind"> (un enfant)</klr></trd>
  <trd lng="nl">opvoeden<klr tip="ind"> (thuis)</klr></trd>
  <trd lng="ru">&c_v;&c_o;&c_s;&c_p;&c_i;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_n;&c_je; &c_s;&c_v;&c_o;&c_je;&c_g;&c_o; &c_r;&c_je;&c_b;&c_jo;&c_n;&c_k;&c_a;)</klr></trd>
</drv>

<drv mrk="guvern.0isto">
  <kap><tld/>ist(in)o</kap>
  <snc>
    <dif>
      Vir(in)o, kies profesio estas <tld/>i.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Erzieher(in)</trd>,
    <trd>Hauslehrer(in)</trd>,
    <trd>Gouvernante</trd>
  </trdgrp>
  <trd lng="en">tutor</trd>
  <trdgrp lng="fr">
    <trd>pr&eacute;cepteur</trd>,
    <trd>gouvernante</trd>
  </trdgrp>
  <trd lng="nl">gouvernante</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_u;&c_v;&c_je;&c_r;&c_n;&c_jo;&c_r;</trd>,
    <trd>&c_g;&c_u;&c_v;&c_je;&c_r;&c_n;&c_a;&c_n;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
