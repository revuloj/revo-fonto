<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>guvern</rad>/i
</kap>
<drv mrk="guvern.0i">
<kap><ofc>*</ofc><tld/>i</kap>
<gra><vspec>tr</vspec></gra>
<snc>
<dif>
Direkti k prizorgi la edukadon de infano, kunvivante kun &gcirc;i.
</dif>
</snc>
<trd lng="en">tutor</trd>
<trd lng="ru">&c_v;&c_o;&c_s;&c_p;&c_i;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_n;&c_je; &c_s;&c_v;&c_o;&c_je;&c_g;&c_o; &c_r;&c_je;&c_b;&#1105;&c_n;&c_k;&c_a;)</klr></trd>
</drv>
<drv mrk="guvern.0isto">
<kap><tld/>ist(in)o</kap>
<snc>
<dif>
Vir(in)o, kies profesio estas <tld/>i.
</dif>
</snc>
<trd lng="en">tutor</trd>
<trdgrp lng="ru">
  <trd>&c_g;&c_u;&c_v;&c_je;&c_r;&c_n;&#1105;&c_r;</trd>,
  <trd>&c_g;&c_u;&c_v;&c_je;&c_r;&c_n;&c_a;&c_n;&c_t;&c_k;&c_a;</trd>
</trdgrp>
</drv>
</art>

