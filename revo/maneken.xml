<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: maneken.xml,v 1.15 2024/01/25 20:10:34 revo Exp $">
<kap>
  <rad>maneken</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="maneken.0o">
  <kap><tld/>o</kap>
  <snc mrk="maneken.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Homoforma, ordinare homegranda <ref tip="super" cel="pup.0o.ludilo">pupo</ref> uzata kiel
      prezentilo de vestoj, modelo por desegnado...:
      <ekz>
        la virino aparte rigardas la <tld/>on kaj sin vidas kvaza&ubreve; en 
        spegulo
        <fnt>
          <aut>G. R. Ledon</aut>,
          <vrk>Misa mondo</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2003a, numero 2a, p. 9a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj kun rozkoloraj oreloj rigardis nin el vitrinoj
        <fnt>
          <aut>I. Ilf, E. Petrov, trad. V. Okc</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Usono/Enhavo.html">
          Unueta&gcirc;a Usono</url></vrk>,
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_a;&c_n;&c_e;&c_k;&c_je;&c_n; <klr>(&c_p;&c_r;&c_y;&c_l;&c_a;&c_d;&c_a;)</klr></trd>
    <trd lng="bg">&c_m;&c_a;&c_n;&c_je;&c_k;&c_je;&c_n;</trd>
    <trdgrp lng="ca">
      <trd>maniqu&iacute;</trd>,
      <trd>figur&iacute;</trd>
    </trdgrp>
    <trd lng="cs">manek&yacute;n</trd>
    <trdgrp lng="de">
      <trd>Model</trd>,
      <trd>Mannequin</trd>
    </trdgrp>
    <trd lng="en">mannequin</trd>
    <trd lng="es">maniqu&iacute;</trd>
    <trd lng="it">manichino</trd>
    <trd lng="nl">mannequin</trd>
    <trd lng="pl">manekin</trd>
    <trd lng="ro">manechin</trd>
    <trdgrp lng="sk">
      <trd>figur&iacute;na</trd>,
      <trd>manek&yacute;n</trd>
    </trdgrp>
  </snc>
  <snc mrk="maneken.0o.TEKS_homo">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="dif" cel="model.0ulo.TEKS">modelulo</ref>
      <ekz>
        la <tld/>oj pa&scirc;etis sur la scenejon en pi&jcirc;amoj
        <fnt>
          <aut>C. Rossetti</aut>,
          <vrk>Kredu Min, Sinjorino!</vrk>,
          <lok>1950</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_m;&c_a;&c_n;&c_e;&c_k;&c_je;&c_n;&c_sh;&c_ch;&c_y;&c_k;</trd>,
      <trd>&c_m;&c_a;&c_d;&c_e;&c_l;&c_mol; <klr>(&c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;)</klr></trd>
    </trdgrp>
    <trd lng="en">model<klr tip="ind"> (person)</klr></trd>
    <trd lng="es">modelo</trd>
    <trd lng="nl">model<klr tip="ind"> (persoon)</klr></trd>
    <trd lng="pl">model</trd>     
    <trd lng="ro">model</trd>
  </snc>
  <trd lng="fr">mannequin</trd>
  <trdgrp lng="ja">
    <trd>&#x30DE;&#x30CD;&#x30AD;&#x30F3;</trd>,
    <trd>&#x30DE;&#x30CC;&#x30AB;&#x30F3;</trd>,
    <trd>&#x30D5;&#x30A1;&#x30C3;&#x30B7;&#x30E7;&#x30F3;&#x30E2;&#x30C7;&#x30EB;</trd>,
    <trd>&#x4EBA;&#x4F53;&#x6A21;&#x578B;  <pr>&#x3058;&#x3093;&#x305F;&#x3044;&#x3082;&#x3051;&#x3044;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_a;&#x43D;&c_je;&#x43A;&c_je;&#x43D;</trd>,
    <trd>&#x43C;&c_a;&#x43D;&c_je;&#x43A;&c_je;&#x43D;&#x43D;&c_i;&c_c;&#x44F;</trd>
  </trdgrp>
  <trd lng="zh">&#x6A21;&#x7279; <pr>m&#xF3;t&#xE8;</pr></trd>
</drv>
</art>

