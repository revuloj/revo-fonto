<vortaro>
<art mrk="$Id: skalp.xml,v 1.18 2023/09/03 18:39:54 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>skalp</rad>/o
</kap>
<drv mrk="skalp.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">FIZL</uzo>
    <dif>
      <ref tip="malprt" cel="hauxt.0o.ANA">Ha&ubreve;to</ref>
      de la kranio &ccirc;irka&ubreve;tran&ccirc;ita kaj for&scirc;irita kun la haroj,
      kiun la nordamerikaj indianoj portis kiel militan trofeon:
      <ekz>
        <tld/>ado a&ubreve; <tld/>umo estas la akcidenta a&ubreve; intenca fortirado a&ubreve; detran&ccirc;ado de
        la <tld/>o, la kraniha&ubreve;to
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=250217">Skalpado</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_a;&c_l;&c_mol;&c_p;</trd>
  <trd lng="ca">cuir cabellut</trd>
  <trd lng="cs">skalp</trd>
  <trd lng="de">Skalp</trd>
  <trd lng="en">scalp</trd>
  <trd lng="fr">scalp</trd>
  <trd lng="hu">skalp</trd>
  <trd lng="ja">&#x6226;&#x5229;&#x982D;&#x76AE;  [<ind>&#x3044;&#x304F;&#x3055;&#x308A;&#x3042;&#x305F;&#x307E;&#x304B;&#x308F;</ind>]</trd>
  <trd lng="nl">scalp</trd>
  <trd lng="pl">skalp</trd>
  <trd lng="ru">&c_s;&c_k;&c_a;&c_l;&c_mol;&c_p;</trd>
  <trd lng="sk">skalp</trd>
  <trd lng="uk">&c_s;&#x43A;&c_a;&#x43B;&#x44C;&#x43F;</trd>
</drv>
</art>
