<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>ekstaz</rad>/o
</kap>
<drv mrk="ekstaz.0o">
<kap><tld/>o</kap>
<snc>
<dif>
Ravi&gcirc;o de la animo, kiu momente kvaza&ubreve; perdas konscion:
<ekz>pre&gcirc;a, ama, admira <tld/>o;</ekz>
<ekz>
  <ind><tld/>a</ind> rideto.
  <trd lng="be">&c_e;&c_k;&c_s;&c_t;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="de">
     <trd>ekstatisch</trd>,
     <trd>verz&uuml;ckt</trd>
  </trdgrp>
  <trd lng="fr">extatique</trd>
</ekz>
</dif>
<adm>
"La animo perdas konscion" ? A&ubreve; eble la ravi&gcirc;o
perdigas konscion. <aut>MB</aut>
</adm>
<refgrp tip="vid">
<ref cel="entuzi.0o">entuziasmo</ref>.
</refgrp>
</snc>
<trd lng="be">&c_e;&c_k;&c_s;&c_t;&c_a;&c_z;</trd>
  <trdgrp lng="de">
     <trd>Ekstase</trd>,
     <trd>Verz&uuml;ckung</trd>
  </trdgrp>
<trd lng="es">&eacute;tasis</trd>
<trd lng="fr">extase</trd>
<trdgrp lng="hu">
  <trd>ext&aacute;zis</trd>,
  <trd>elragadtat&aacute;s</trd>
</trdgrp>
<trdgrp lng="ru">
  <trd>&c_e;&c_k;&c_s;&c_t;&c_a;&c_z;</trd>,
  <trd>&c_i;&c_s;&c_s;&c_t;&c_u;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
  <trd>&c_v;&c_o;&c_s;&c_t;&c_o;&c_r;&c_g;</trd>
</trdgrp>
</drv>
<drv mrk="ekstaz.0i">
<kap><tld/>i</kap>
<gra><vspec>ntr</vspec></gra>
<snc>
<dif>
Esti en stato de <tld/>o.  
</dif>
</snc>
<trd lng="be">&c_b;&c_y;&c_c;&c_mol; &c_u; <ind>&c_e;&c_k;&c_s;&c_t;&c_a;&c_z;&c_je;</ind></trd>
  <trdgrp lng="de">
     <trd>in <ind>Ekstase</ind> sein</trd>,
     <trd><ind>verz&uuml;ckt</ind> sein</trd>
  </trdgrp>
<trdgrp lng="fr">
  <trd>&ecirc;tre en <ind>extase</ind></trd>,
  <trd>s'<ind>extasier</ind></trd></trdgrp> 
<trdgrp lng="hu">
  <trd>ext&aacute;zisban van</trd>,
  <trd>ext&aacute;zisba esett</trd>
</trdgrp>
<trd lng="ru">&c_b;&c_y;&c_t;&c_mol; &c_v; &c_e;&c_k;&c_s;&c_t;&c_a;&c_z;&c_je;</trd>
</drv>
</art>
