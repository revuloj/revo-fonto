<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: drone.xml,v 1.5 2023/11/04 18:50:52 revo Exp $">
<kap>
    <rad>drone</rad>/o
</kap>
<drv mrk="drone.0o">
  <kap><tld/>o</kap>
  <snc mrk="drone.0o.SNC">
    <uzo tip="fak">MAS</uzo>
    <dif>
      Senpilota <ref tip="super" cel="masxin.flug0o">flugma&scirc;ino</ref>,
      kiu
      estas de distanco stirata a&ubreve; tute a&ubreve;tomata:
      <ekz>
        la franca aerarmeo trejnas aglojn kun la celo &rightquot;&ccirc;asi&#x201D;
        suspektindajn etajn telegvidatajn aviadilojn, la tiel nomatajn
        <tld/>ojn
        <fnt>
          <aut>Juan Carlos Montero</aut>
          <vrk><url ref="&Monato;/2017/011961.php">
            Agloj povas kapti droneojn</url></vrk>
          <bib>Monato</bib>
          <lok/>
        </fnt>;
      </ekz>
      <ekz>
        ho, <tld/>o forfluganta!
        <fnt>
          <aut>Laure Patas d'Illiers</aut>
          <vrk>Servi vin</vrk>
          <lok><url ref="https://books.google.nl/books?id=QTOGDwAAQBAJ">
Belarta Rikolto 2018: Premiitaj Verkoj de la Belartaj Konkursoj de
Universala Esperanto-Asocio</url>, pa&gcirc;o 47a</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i havos 8 duoblajn rotorojn en kvadrato, same kiel komerca
        <tld/>o
        <fnt>
          <aut>Frank van Hertrooij</aut>
          <vrk><url ref="https://scivolemo.wordpress.com/2019/09/18/esplori-la-kosmon-per-helikoptero/">
            Esplori la kosmon per helikoptero</url></vrk>
          <lok>Scivolemo, 2019-09-18</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="spav.0o">spavo</ref>
    <refgrp tip="super">
      <ref cel="robot.0o">roboto</ref>, 
      <ref cel="aviad.0ilo">aviadilo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_s;&c_p;&c_ib;&c_l;&c_o;&c_t;&c_n;&c_ib;&c_k;</trd>
  <trd lng="ca">dron</trd>
  <trd lng="cs">dron</trd>
  <trd lng="da">drone</trd>
  <trd lng="de">Drohne</trd>
  <trdgrp lng="en">
    <trd>drone</trd>,
    <trd>unmanned aerial vehicle</trd>,
    <trd>UAV</trd>
  </trdgrp>
  <trd lng="es">dron</trd>
  <trd lng="et">droon</trd>
  <trd lng="fa">&f_p;&a_h;&f_p;&a_A;&a_d;</trd>
  <trdgrp lng="fi">
    <trd>drone</trd>,
    <trd>drooni</trd>
  </trdgrp>
  <trd lng="fr">drone</trd>
  <trd lng="id">dron</trd>
  <trd lng="it">drone</trd>
  <trd lng="hu">dr&oacute;n</trd>
  <trdgrp lng="ja">
    <trd>&#x30C9;&#x30ED;&#x30FC;&#x30F3; <pr>&#x3069;&#x308D;&#x30FC;&#x3093;</pr></trd>,
    <trd>&#x7121;&#x4EBA;&#x98DB;&#x884C;&#x6A5F; <pr>&#x3080;&#x3058;&#x3093;&#x3072;&#x3053;&#x3046;&#x304D;</pr></trd>
  </trdgrp>
  <trdgrp lng="ko">
    <trd>&#xB4DC;&#xB860;</trd>
    <trd>&#xBB34;&#xC778;&#xD56D;&#xACF5;&#xAE30;</trd>
  </trdgrp>
  <trd lng="nl">drone</trd>
  <trd lng="pl">dron</trd>
  <trd lng="pt">drone</trd>
  <trd lng="ro">dron&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_s;&c_p;&c_i;&c_l;&c_o;&c_t;&c_n;&c_i;&c_k;</trd>
    <trd>&c_d;&c_r;&c_o;&c_n;</trd>
  </trdgrp>
  <trd lng="tr">dron</trd>
  <trd lng="sv">dr&ouml;nare</trd>
  <trdgrp lng="zh">
    <trd>&#x65E0;&#x4EBA;&#x98DE;&#x884C;&#x8F7D;&#x5177; <pr>w&#xFA;r&#xE9;nf&#x113;ih&#xE1;ngz&#xE0;ij&#xF9;</pr></trd>,
    <trd>&#x65E0;&#x4EBA;&#x9A7E;&#x9A76;&#x98DE;&#x673A; <pr>w&#xFA;r&#xE9;nji&#xE0;sh&#x1D0;f&#x113;ij&#x12B;</pr></trd>
  </trdgrp>
</drv>
</art>

