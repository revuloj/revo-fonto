<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>nev</rad>/o
</kap>
<drv mrk="nev.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">GEN</uzo>
  <snc>
    <dif>
      Filo de frato a&ubreve; fratino.
    </dif>
  </snc>                         
  <trd lng="be">&c_p;&c_l;&c_ja;&c_m;&c_je;&c_n;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trd lng="br">niz</trd>
  <trd lng="cs">synovec</trd>
  <trd lng="de">Neffe</trd>
  <trd lng="el">&alfa;&ny;&eta;&psi;&jota;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">nephew</trd>
  <trd lng="es">sobrino</trd>
  <trd lng="fr">neveu</trd>
  <trd lng="he">&alef;&het;&yod;&yod;&fnun;</trd>
  <trd lng="hu">unoka&ouml;ccs</trd>
  <trd lng="nl">neef<klr tip="ind"> (zoon v. broer of zus)</klr></trd>
  <trd lng="pt">sobrinho</trd>
  <trd lng="ru">&c_p;&c_l;&c_je;&c_m;&c_ja;&c_n;&c_n;&c_i;&c_k;</trd>
  <trdgrp lng="sv">
    <trd>syskonbarn</trd>,
    <trd>brorson</trd>,
    <trd>systerson</trd>
  </trdgrp>
</drv>

<drv mrk="nev.0ino">
  <kap><tld/>ino</kap>
  <uzo tip="fak">GEN</uzo>
  <snc>
    <dif>
      Filino de frato a&ubreve; fratino.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ja;&c_m;&c_je;&c_n;&c_mol;&c_n;&c_ib;&c_c;&c_a;</trd>  
  <trd lng="br">nizez</trd>
  <trd lng="en">niece</trd>  
  <trd lng="fr">ni&egrave;ce</trd>
  <trd lng="hu">unokah&uacute;g</trd>
  <trd lng="ru">&c_p;&c_l;&c_je;&c_m;&c_ja;&c_n;&c_n;&c_i;&c_c;&c_a;</trd>  
</drv>

</art>
