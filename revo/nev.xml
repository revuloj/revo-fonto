<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nev.xml,v 1.32 2023/09/02 12:44:21 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>nev</rad>/o
</kap>
<drv mrk="nev.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">GEN</uzo>
  <snc>
    <subsnc mrk="nev.0o.frata">
      <dif>
        <ref tip="lst" cel="fil.0o" lst="voko:parencoj">Filo</ref> de
        frato.
      </dif>
      <trd lng="pl">bratanek</trd>
      <trd lng="vo">blodason</trd>
    </subsnc>
    <subsnc mrk="nev.0o.fratina">
      <dif>
        <ref tip="lst" cel="fil.0o" lst="voko:parencoj">Filo</ref> de
        fratino.
      </dif>
      <trd lng="pl">siostrzeniec</trd>
      <trd lng="vo">s&ouml;rason</trd>
    </subsnc>
    <ekz>
      la edzino de mia <tld/>o kaj la <tld/>ino de mia edzino estas miaj bo<tld/>inoj
      <fnt>
        <bib>F</bib>
        <lok>&FE; 36</lok>
      </fnt>;
    </ekz>
    <ekz>
      kiel fartas nia kara <tld/>o Hamleto?
      <fnt>
        <bib>Hamlet</bib>
      </fnt>;
    </ekz>
    <ekz>
      nevo de papo facile fari&gcirc;as kardinalo
      <fnt>
        <bib>PrV</bib>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ja;&c_m;&c_je;&c_n;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trd lng="bg">&c_p;&c_l;&c_je;&c_m;&c_je;&c_n;&c_n;&c_i;&c_k;</trd>
  <trd lng="br">niz</trd>
  <trd lng="cs">synovec</trd>
  <trd lng="de">Neffe</trd>
  <trd lng="el">&alfa;&ny;&eta;&psi;&jota;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">nephew</trd>
  <trd lng="es">sobrino</trd>
  <trdgrp lng="fa">
    <trd>&a_kh;&a_w;&a_A;&a_h;&a_r;&a_z;&a_A;&a_d;&a_h;</trd>,
    <trd>&a_b;&a_r;&a_A;&a_d;&a_r;&a_z;&a_A;&a_d;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">neveu</trd>
  <trd lng="fy">neef</trd>
  <trd lng="he">&alef;&het;&yod;&yod;&fnun;</trd>
  <trd lng="hu">unoka&ouml;ccs</trd>
  <trd lng="io">nevulo</trd>
  <trd lng="ja">&#x304A;&#x3044;</trd>
  <trd lng="nl">neef<klr tip="ind"> (zoon van broer of zus)</klr></trd>
  <trd lng="pt">sobrinho</trd>
  <trd lng="ru">&c_p;&c_l;&c_je;&c_m;&c_ja;&c_n;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">synovec</trd>
  <trdgrp lng="sv">
    <trd>syskonbarn<klr> (pojke)</klr></trd>,
    <trd>brorson</trd>,
    <trd>systerson</trd>
  </trdgrp>
  <trd lng="tr">erkek ye&gbreve;en</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43B;&#x435;&#x43C;&#x456;&#x43D;&#x43D;&#x438;&#x43A;</trd>,
    <trd>&#x43D;&#x435;&#x431;&#x456;&#x436;</trd>
  </trdgrp>
  <trd lng="vo">hinef</trd>
</drv>

<drv mrk="nev.0ino">
  <kap><tld/>ino</kap>
  <uzo tip="fak">GEN</uzo>
  <snc>
    <subsnc>
      <dif>
        <ref tip="lst" cel="fil.0ino" lst="voko:parencoj">Filino</ref> de
        frato.
      </dif>
      <trd lng="pl">bratanica</trd>
      <trd lng="vo">blodadaut</trd>
    </subsnc>
    <subsnc>
      <dif>
        <ref tip="lst" cel="fil.0ino" lst="voko:parencoj">Filino</ref> de
        fratino.
      </dif>
      <trd lng="pl">siostrzenica</trd>
      <trd lng="vo">s&ouml;radaut</trd>
    </subsnc> 
    <ekz>
      mi vidis vian avinon kun &scirc;iaj kvar nepinoj kaj kun mia <tld/>ino
      <fnt>
        <bib>F</bib>
        <lok>&FE; 33</lok>
      </fnt>;
    </ekz>
    <ekz>
      mian malgrandan <tld/>inon mi povos havi &ccirc;e mi
      <fnt>
        <bib>Marta</bib>
      </fnt>?
    </ekz>
    <ekz>
      &scirc;i &ccirc;irka&ubreve;prenis siajn <tld/>inojn
      <fnt>
        <bib>BdV</bib>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ja;&c_m;&c_je;&c_n;&c_mol;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="bg">&c_p;&c_l;&c_je;&c_m;&c_je;&c_n;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="br">nizez</trd>
  <trd lng="cs">nete&rcaron;</trd>
  <trd lng="de">Nichte</trd>
  <trd lng="en">niece</trd>
  <trd lng="es">sobrina</trd>
  <trd lng="fr">ni&egrave;ce</trd>
  <trd lng="fy">nicht</trd>
  <trd lng="hu">unokah&uacute;g</trd>
  <trd lng="io">nevino</trd>
  <trd lng="ja">&#x3081;&#x3044;</trd>
  <trd lng="nl">nicht<klr tip="ind"> (dochter van broer of
  zus)</klr></trd>
  <trd lng="pt">sobrinha</trd>
  <trd lng="ru">&c_p;&c_l;&c_je;&c_m;&c_ja;&c_n;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">neter</trd>
  <trdgrp lng="sv">
    <trd>syskonbarn<klr> (flicka)</klr></trd>,
    <trd>brorsdotter</trd>,
    <trd>systerdotter</trd>
  </trdgrp>
  <trd lng="tr">k&inodot;z ye&gbreve;en</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43B;&#x435;&#x43C;&#x456;&#x43D;&#x43D;&#x438;&#x446;&#x44F;</trd>,
    <trd>&#x43D;&#x435;&#x431;&#x43E;&#x433;&#x430;</trd>
  </trdgrp>
  <trd lng="vo">jinef</trd>
</drv>

</art>
