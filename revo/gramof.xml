<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>6</ofc>
  <rad>gramofon</rad>/o
</kap>
<drv mrk="gramof.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="fonogr.0o">Fonograf-aparato</ref>, reproduktanta la sonojn per diskoj.  
    </dif>
  </snc>
  <adm>
  Kial "fonograf-aparato" kaj ne "fonografo"?
  Vd adm sub "fonograf". La trd fr estas 
  provizora. <aut>MB</aut>
  </adm>
  <trdgrp lng="de">
    <trd>Plattenspieler</trd>,
    <trd>Grammophon <klr>(arkaika)</klr></trd>,
    <trd>CD-Spieler</trd>
  </trdgrp>
  <trd lng="fr">gramophone</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_a;&c_m;&c_m;&c_o;&c_f;&c_o;&c_n; <klr>(arkaika)</klr></trd>,
    <trd>&c_p;&c_r;&c_o;&c_i;&c_g;&c_r;&c_y;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sv">
     <trd>grammofon</trd>,
     <trd>skivspelare</trd>
  </trdgrp>
  </drv>
</art>
