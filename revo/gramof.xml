<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>6</ofc>
  <rad>gramofon</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="gramof.0o">
  <kap><tld/>o</kap>
  <snc mrk="gramof.0o.SON">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Aparato kiu povas reprodukti sonon el bendo, disko 
      a&ubreve; alia registra&jcirc;o:
      <ekz>
        neniam vi sukcesos, ke la homoj abonu radioa&ubreve;dadon. Ili jam
        havas
        <tld/>ojn
        <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro 55a, p.
        124a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_r;&c_a;&c_m;&c_a;&c_f;&c_o;&c_n;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_j;&c_g;&c_n;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;
    <klr>(&c_k;&c_r;&c_u;&c_zh;&c_e;&c_l;&c_a;&c_k;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Plattenspieler</trd>,
    <trd>Grammophon <klr>(arkaika)</klr></trd>,
    <trd>CD-Spieler</trd>
  </trdgrp>
  <trd lng="es">gram&oacute;fono</trd>
  <trdgrp lng="fr">
    <trd>chaine<klr> (mus.)</klr></trd>,
    <trd>gramophone</trd>,
    <trd>tourne-disque<klr> (mus.)</klr></trd>
  </trdgrp>
  <trd lng="hu">gramofon</trd>
  <trdgrp lng="nl">
    <trd>platenspeler</trd>,
    <trd>grammofoon</trd>
  </trdgrp>
  <trd lng="pt">gramofone</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_a;&c_m;&c_m;&c_o;&c_f;&c_o;&c_n; <klr>(arkaika)</klr></trd>,
    <trd>&c_p;&c_r;&c_o;&c_i;&c_g;&c_r;&c_y;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>grammofon</trd>,
    <trd>skivspelare</trd>
    <trd lng="tr">gramafon</trd>
  </trdgrp>
</drv>
</art>
