<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sublim.xml,v 1.21 2023/11/21 01:39:28 revo Exp $">
<kap>
  <rad>sublim</rad>/a <fnt><aut>K. Bennemann</aut>, <vrk>Esperanto
  Hand-W&ouml;rterbuch</vrk>, <lok>1923-1926</lok></fnt>
</kap>
<drv mrk="sublim.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="sublim.0a.KOMUNE">
    <dif>
      Distingita per sia beleco, nobleco, sankteco; kvaza&ubreve;religie inspira, admirinda:
      <ekz>
        spiriteco estas rezulto de ekzaltaj, <tld/>aj momentoj travivitaj
        <fnt>
          <bib>Monato</bib>
          <aut>W. H. Simcock</aut>
          <vrk><url ref="&Monato;/2007/009197.php">Kio estas spiriteco?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Tenere a&ubreve; lascive / Mi strebas volitive / Ascendi al <tld/>a sfer', /
        Eskapi el obscena strat' / Tra virga spac' de l' stratosfer
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        religi (unuigi) estas unu el la plej <tld/>aj esprimoj de kulturo
        <fnt>
          <bib>Monato</bib>
          <aut>Jo Haazen</aut>
          <vrk><url ref="&Monato;/2013/010773.php">Klareco kaj simpleco, fundamento por nova
          epoko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>(ironie)</klr>
        kuzo <klr>[&#8230;]</klr> suspektas lian unikan virton, lian <tld/>an &leftquot;senpekan&#x201D; korpamon
        al la baptofilino
        <fnt>
          <bib>Monato</bib>
          <aut>Donald BROADRIBB</aut>
          <vrk><url ref="&Monato;/2003/007837.html">Ne al &ccirc;iuj pla&ccirc;os &ccirc;i literatura
          gemeto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_u;&c_z;&c_mol;&c_n;&c_jo;&c_s;&c_l;&c_y;</trd>,
      <trd>&c_v;&c_je;&c_l;&c_ib;&c_ch;&c_n;&c_y;</trd>,
      <trd>&c_g;&c_r;&c_a;&c_n;&c_d;&c_y;&c_jo;&c_z;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="cs">
      <trd>grandi&oacute;zn&iacute;</trd>,
      <trd>skv&ecaron;l&yacute;</trd>,
      <trd>vzne&scaron;en&yacute;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>majest&auml;tisch</trd>,
      <trd>erhaben</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>noble</trd>,
      <trd>majestueux</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>gy&ouml;ny&ouml;r&udblac;</trd>,
      <trd>k&aacute;pr&aacute;zatos</trd>
      <trd>fens&eacute;ges</trd>,
      <trd>magasztos</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>subliem</trd>,
      <trd>verheven</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>podnios&lstroke;y</trd>,
      <trd>szczytny</trd>,
      <trd>wznios&lstroke;y</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_je;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_g;&c_r;&c_a;&c_n;&c_d;&c_i;&c_o;&c_z;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_v;&c_o;&c_z;&c_v;&c_y;&c_sh;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>grandi&oacute;zny</trd>,
      <trd>skvel&yacute;</trd>,
      <trd>vzne&scaron;en&yacute;</trd>,
      <trd>&scaron;&lcaron;achten&yacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sublim.0a.delikata">
    <dif>
       Rimarkebla nur per tre delikataj sentoj; <ref tip="vid"
       cel="subtil.0a.distinge">subtila<sncref/></ref>.
    </dif>
    <trdgrp lng="de">
      <trd>sublim</trd>,
      <trd>unterschwellig</trd>
    </trdgrp>
    <trd lng="fr">sublime</trd>
    <trd lng="pl">wysublimowany</trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x5D07;&#x9AD8;&#x306A;  <pr>&#12377;&#12358;&#12371;&#12358;&#x306A;</pr></trd>,
    <trd>&#x81F3;&#x9AD8;&#x306E;  <pr>&#12375;&#12371;&#12358;&#x306E;</pr></trd>,
    <trd>&#x6C17;&#x9AD8;&#12356;  <pr>&#12369;&#12384;&#x304B;&#12356;</pr></trd>,
    <trd>&#x9AD8;&#x5C1A;&#x306A;  <pr>&#12371;&#12358;&#12375;&#12423;&#12358;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_ib;&c_d;&#x43D;&c_je;&c_s;&c_je;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_i;&c_s;&#x43E;&#x43A;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_je;&#x43B;&c_i;&c_ch;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
