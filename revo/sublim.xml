      <trd>&#39640;&#23578; <pr>g&amacron;osh&#224;ng</pr></trd>
    </trdgrp>
  </snc>
  <snc mrk="sublim.0a.delikata">
    <dif>
       Rimarkebla nur per tre delikataj sentoj; <ref tip="vid" cel="subtil.0a.distinge">subtila<sncref/></ref>.
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_t;&c_a;&c_n;&c_ch;&c_a;&c_n;&c_y;</trd>,
