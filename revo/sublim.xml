      <trd>wznios&lstroke;y</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_je;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_g;&c_r;&c_a;&c_n;&c_d;&c_i;&c_o;&c_z;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_v;&c_o;&c_z;&c_v;&c_y;&c_sh;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>grandi&oacute;zny</trd>,
      <trd>skvel&yacute;</trd>,
      <trd>vzne&scaron;en&yacute;</trd>,
      <trd>&scaron;&lcaron;achten&yacute;</trd>
    </trdgrp>
    <trdgrp lng="zh">
      <trd>&#23041;&#39080;&#20956;&#20956; <pr>w&emacron;if&emacron;ngl&#464;nl&#464;n</pr></trd>,
      <trd>&#23041;&#39118;&#20955;&#20955; <pr>w&emacron;if&emacron;ngl&#464;nl&#464;n</pr></trd>,
      <trd>&#24196;&#20005; <pr>zhu&amacron;ngy&#225;n</pr></trd>,
      <trd>&#33674;&#22196; <pr>zhu&amacron;ngy&#225;n</pr></trd>,
      <trd>&#23041;&#39080; <pr>w&emacron;if&emacron;ng</pr></trd>,
      <trd>&#23041;&#39118; <pr>w&emacron;if&emacron;ng</pr></trd>,
      <trd>&#23815;&#39640; <pr>ch&#243;ngg&amacron;o</pr></trd>,
