<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>hierarki</rad>/o
</kap>
<drv mrk="hierarki.0o">
  <kap><tld/>o</kap>
  <snc mrk="hierarki.0o.POL">
    <dif>
      Aran&gcirc;o de rangoj, oficoj, postenoj la&ubreve; grada ordo:
      <ekz>
	tio fine ka&ubreve;zos publikan konflikton inter la re&gcirc;o kaj la tuta
	<tld/>o
	<fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 18a, maltrankvilo en la re&gcirc;a
	palaco</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="hierar.0o">hierar&hcirc;io</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_ib;&c_je;&c_r;&c_a;&c_r;&c_h;&c_ib;&c_ja;</trd>,
    <trd>&c_g;&c_ib;&c_je;&c_r;&c_a;&c_r;&c_h;&c_ib;&c_ja;</trd>
  </trdgrp>
  <trd lng="de">Hierarchie</trd>
  <trd lng="el">&jota;&epsilon;&rho;&alfa;&rho;&chi;&jota_ton;&alfa;</trd>
  <trd lng="en">hierarchy</trd>
  <trd lng="es">jerarqu&iacute;a</trd>
  <trd lng="fr">hi&eacute;rarchie</trd>
  <trdgrp lng="hu">
    <trd>rangsor</trd>,
    <trd>hierarchia</trd>
  </trdgrp>
  <trd lng="it">gerarchia</trd>
  <trd lng="nl">hierarchie</trd>
  <trd lng="pl">hierarchia</trd>
  <trd lng="pt">hierarquia</trd>
  <trd lng="ru">&c_i;&c_je;&c_r;&c_a;&c_r;&c_h;&c_i;&c_ja;</trd>
</drv>
</art>

