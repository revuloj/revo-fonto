  <trd lng="de">Pal&auml;oanthropologe</trd>
  <trdgrp lng="fr">
    <trd>pal&eacute;oanthropologiste</trd>,
    <trd>pal&eacute;oanthropologue</trd>
  </trdgrp>
  <trd lng="nl">paleoantropoloog</trd>
  <trd lng="pl">paleoantropolog</trd>
  <trd lng="ro">paleoantropolog</trd>
  <trd lng="uk">&#x43F;&c_a;&#x43B;&c_je;&#x43E;&c_a;&#x43D;&c_t;&c_r;&#x43E;&#x43F;&#x43E;&#x43B;&#x43E;&c_g;</trd>
  <trdgrp lng="zh">
    <trd>&#x53E4;&#x751F;&#x7269;&#x5B66;&#x5BB6; <pr>g&#x1D4;sh&#x113;ngw&#xF9;xu&#xE9;ji&#x101;</pr></trd>,
