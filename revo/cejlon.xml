    <kap>
      <rad>Cejlon</rad>/o
    </kap>

<drv mrk="cejlon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Iama nomo, &gcirc;is 1972, de <ref tip="dif" cel="srilank.0o">Srilanko</ref>:
      <ekz>
        iama Siamo nun estas Tajlando kaj Sri-Lanko anta&ubreve;e nomi&gcirc;is <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2007/009340.php">Bizara lando</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum mia restado trimonata en <tld/>o, mi ofte renkontis <klr>[&#8230;]</klr> homojn,
        kiuj &leftquot;a&ubreve;dis ion pri Esperanto &ndash; anta&ubreve; multaj jaroj&rightquot; <klr>[&#8230;]</klr> a&ubreve; sciis,
        ke &leftquot;multaj <tld/>anoj interesi&gcirc;is pri &gcirc;i anta&ubreve; la milito&rightquot;
        <fnt>
          <aut>Victor Sadler</aut>
          <vrk><url ref="&Tekstaro;?nomo=azia-strategio">Azia Strategio</url></vrk>
          <lok>Revuo Esperanto, 1963</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Ceylon</trd>
    <trd lng="en">Ceylon</trd>
  </snc>
  <trd lng="be">&c_C;&c_e;&c_j;&c_l;&c_jo;&c_n;</trd>
  <trd lng="he">&#x5E6;&#x5D9;&#x5D9;&#x5DC;&#x5D5;&#x5DF;</trd>
  <trd lng="ja">&#x30BB;&#x30A4;&#x30ED;&#x30F3;&#x5CF6; 
  <pr>&#x30BB;&#x30A4;&#x30ED;&#x30F3;&#12375;&#x307E;</pr></trd>
  <trd lng="pl">Cejlon</trd>
  <trd lng="uk">&c_C;&c_je;&c_j;&#x43B;&#x43E;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x932B;&#x862D; <pr>x&#xED;l&#xE1;n</pr></trd>,
    <trd>&#x72EE;&#x5B50;&#x56FD; <pr>sh&#x12B;zigu&#xF3;</pr></trd>
  </trdgrp>
</drv>
