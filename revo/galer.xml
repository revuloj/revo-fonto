<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>galer</rad>/o
</kap>
<drv mrk="galer.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Rema kaj vela milit&scirc;ipo en antikva kaj meza epokoj.
    </dif>
    <trd lng="de">Galeere</trd>
    <trd lng="nl">galei</trd>
    <trd lng="ru">&c_g;&c_a;&c_l;&c_je;&c_r;&c_a;</trd>
  </snc>
  <snc>
    <uzo tip="fak">JUR</uzo>
    <dif>
      Samspeca &scirc;ipo uzata por punlaboro pro gravaj krimoj.
    </dif>
    <trd lng="nl">galei</trd>
    <trd lng="ru">&c_g;&c_a;&c_l;&c_je;&c_r;&c_a;</trd>
  </snc>
  <snc>
    <uzo tip="fak">JUR</uzo>
    <dif>
      Punlaboro:
      <ekz>
        kondamnita je <tld/>oj.
      </ekz>
    </dif>
    <trd lng="nl">dwangarbeid</trd>
    <trd lng="ru">&c_k;&c_a;&c_t;&c_o;&c_r;&c_g;&c_a;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ege peniga laboro.
    </dif>
    <trd lng="ru">&c_k;&c_a;&c_t;&c_o;&c_r;&c_g;&c_a;</trd>
  </snc>
  <trd lng="fr">gal&egrave;re</trd>
</drv>
</art>
