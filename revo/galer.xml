<kap>
  <ofc>1</ofc>
  <rad>galer</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="galer.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Rema kaj vela milit&scirc;ipo en antikva kaj meza epokoj:
      <ekz>
        Kun Johan' rapide iris la <tld/>o, /
        Sin al vent' apogis &gcirc;ia lar&gcirc;a velo
        <fnt>
          <aut>S. Pet&odblac;fi, trad. K. Kalocsay</aut>,
          <vrk><url ref="http://www.egalite.hu/kalocsay/petofi/johano%20la%20brava.htm">
          Johano la brava</url></vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_g;&c_a;&c_l;&c_je;&c_r;&c_a;</trd>
    <trd lng="de">Galeere</trd>
    <trd lng="en">galley</trd>
    <trd lng="nl">galei</trd>
    <trd lng="ru">&c_g;&c_a;&c_l;&c_je;&c_r;&c_a;</trd>
  </snc>
  <snc>
    <uzo tip="fak">JUR</uzo>
    <dif>
      Samspeca &scirc;ipo uzata por punlaboro pro gravaj krimoj:
            <ekz>
        li meritas ripozon, laborinte kiel <tld/>osklavo
        <fnt>
          <aut>Heinrich August Luyken</aut>,
          <vrk>Stranga hereda&jcirc;o</vrk>,
          <lok><url ref="&Gutenberg;ebooks/55954">
        gutenberg.org</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_g;&c_a;&c_l;&c_je;&c_r;&c_a;</trd>
    <trd lng="en">galley</trd>
    <trd lng="nl">galei</trd>
    <trd lng="ru">&c_g;&c_a;&c_l;&c_je;&c_r;&c_a;</trd>
    <trd lng="tr">k&uuml;rek cezas&inodot;</trd>
  </snc>
  <snc>
    <uzo tip="fak">JUR</uzo>
    <dif>
      Punlaboro:
      <ekz>
        <ind>kondamnita je <tld/>oj</ind>.
        <trd lng="it">condannato al remo</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_t;&c_a;&c_r;&c_g;&c_a;</trd>
    <trd lng="nl">dwangarbeid</trd>
    <trd lng="ru">&c_k;&c_a;&c_t;&c_o;&c_r;&c_g;&c_a;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ege peniga laboro.
    </dif>
    <trd lng="be">&c_k;&c_a;&c_t;&c_a;&c_r;&c_g;&c_a;</trd>
    <trd lng="ru">&c_k;&c_a;&c_t;&c_o;&c_r;&c_g;&c_a;</trd>
  </snc>
  <trd lng="ca">galera</trd>
  <trdgrp lng="cs">
    <trd>galeje</trd>,
    <trd>gal&eacute;ra</trd>,
    <trd>veslice</trd>
  </trdgrp>
  <trd lng="es">galera</trd>
  <trd lng="fr">gal&egrave;re</trd>
  <trd lng="hu">g&aacute;lya</trd>
  <trd lng="it">galera</trd>
  <trdgrp lng="ja">
    <trd>&#x30AC;&#x30EC;&#x30FC;&#x8239;  <pr>&#x30AC;&#x30EC;&#x30FC;&#x305B;&#x3093;</pr></trd>,
    <trd>&#x82E6;&#x5F79;  <pr>&#x304F;&#x3048;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="pt">gal&eacute;</trd>
  <trdgrp lng="sk">
    <trd>gal&eacute;ra</trd>,
    <trd>veslica</trd>
  </trdgrp>
  <trd lng="tr">&ccedil;ok zor i&scedil;, u&gbreve;ra&scedil;</trd>
  <trdgrp lng="uk">
    <trd>&#x491;&#x430;&#x43B;&#x435;&#x440;&#x430;</trd>,
    <trd>&#x43A;&#x430;&#x442;&#x43E;&#x440;&#x436;&#x43D;&#x435; &#x441;&#x443;&#x434;&#x43D;&#x43E;</trd>,
    <trd>&#x43A;&#x430;&#x442;&#x43E;&#x440;&#x433;&#x430;</trd>,
    <trd>&#x43A;&#x430;&#x442;&#x43E;&#x440;&#x436;&#x43D;&#x456; &#x440;&#x43E;&#x431;&#x43E;&#x442;&#x438;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8239;&#x5167;&#x5EDA;&#x623F; <pr>chu&#xE1;nn&#xE8;ich&#xFA;f&#xE1;ng</pr></trd>,
    <trd>&#x7F57;&#x9A6C;&#x7684;&#x8230; <pr>lu&#xF3;m&#x1CE;deji&#xE0;n</pr></trd>,
    <trd>&#x8230;&#x957F;&#x7528;&#x8247; <pr>ji&#xE0;nzh&#x1CE;ngy&#xF2;ngt&#x1D0;ng</pr></trd>,
    <trd>&#x6D3B;&#x5B57;&#x76D8; <pr>hu&#xF3;z&#xEC;p&#xE1;n</pr></trd>,
    <trd>&#x9577;&#x65B9;&#x6D3B;&#x5B57;&#x76E4; <pr>ch&#xE1;ngf&#x101;nghu&#xF3;z&#xEC;p&#xE1;n</pr></trd>,
    <trd>&#x53E4;&#x4EE3;&#x5E0C;&#x814A; <pr>g&#x1D4;d&#xE0;ix&#x12B;l&#xE0;</pr></trd>,
    <trd>&#x6868;&#x6218;&#x8239; <pr>ji&#x1CE;ngzh&#xE0;nchu&#xE1;n</pr></trd>,
    <trd>&#x53E4;&#x4EE3;&#x7684;&#x6709;&#x6868;&#x5E06;&#x8239; <pr>g&#x1D4;d&#xE0;idey&#x1D2;uji&#x1CE;ngf&#xE1;nchu&#xE1;n</pr></trd>
  </trdgrp>
</drv>
