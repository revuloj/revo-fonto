<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&scirc;rik</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sxrik.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="sxrik.0i.SON">
    <dif>
      Akute kaj tre la&ubreve;te <ref tip="super" cel="kri.0i.vocxi">krii</ref>
      pro doloro, timego, subita &scirc;okego:
      <ekz>
        &gcirc;i <tld/>as tiom la&ubreve;tege, ke tiu, kiu eltiras &gcirc;in, tuj mortas
        <fnt><bib>SatirRak</bib>, <lok>supersti&ccirc;o</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="strid.0i">stridi</ref>
    </refgrp>
  </snc>
  <trd lng="fr">hurler<klr> (de peur etc.)</klr></trd>
  <trdgrp lng="hu">
    <trd>sik&iacute;t</trd>,
    <trd>sikolt</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_i;&c_z;&c_zh;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>

