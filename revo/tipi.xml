<art mrk="$Id: tipi.xml,v 1.9 2022/05/05 17:08:11 revo Exp $">
<kap>
  <rad>tipi</rad>/o
</kap>
<drv mrk="tipi.0o">
  <kap><tld/>o</kap>
  <snc mrk="tipi.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Konusforma tendo kun stangoframo
      tegita de feloj, tradicia
      <ref tip="super" cel="logx.0ejo.ARKI">lo&gcirc;ejo</ref>
      de nordamerikaj ebena&jcirc;aj indianoj:
      <ekz>
        <tld/>o estas uzata &mdash; spite al la &gcirc;enerala
        penso &mdash; nur ekde 2&ndash;300 jaroj
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Tipio">
          tipio</url>, 2011-03-04</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
    <ref cel="jurt.0o.ARKI">jurto</ref>,
      <ref cel="vigvam.0o.ARKI">vigvamo</ref>
    </refgrp>
  </snc>
  <trd lng="bg">&c_t;&c_i;&c_p;&c_i;</trd>
  <trd lng="br">tipi</trd>
  <trd lng="ca">tipi</trd>
  <trdgrp lng="cs">
    <trd>indi&#xE1;nsk&#xFD; stan z k&#x16F;&#x17E;&#xED;</trd>,
    <trd>t&yacute;p&iacute;</trd>
  </trdgrp>
  <trd lng="da">tipi</trd>
  <trd lng="de">tipi</trd>
  <trd lng="en">tipi</trd>
  <trd lng="es">tipi</trd>
  <trd lng="fr">tipi</trd>
  <trd lng="it">tipi</trd>
  <trd lng="nl">tipi</trd>
  <trd lng="pl">tipi</trd>
  <trd lng="pt">tipi</trd>
  <trd lng="sk">t&#xED;p&#xED;</trd>
  <trd lng="sv">tipi</trd>
  <trd lng="uk">&#x442;&#x438;&#x43F;&#x456;</trd>
</drv>
</art>

