<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sel</rad>/o
</kap>
<drv mrk="sel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="segx.0o.meblo">Se&gcirc;o</ref>, kiun oni ligas sur
      la dorso de rajdbesto:
      <ekz>
        li saltis sur la <tld/>on;
      </ekz>
      <ekz>
        &ccirc;evalo blanka, pompe ornamita, kun gravurita <tld/>o kaj kolora
        brido
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 52a, p. 174a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Sattel</trd>
  <trd lng="en">saddle</trd>
  <trdgrp lng="fr">
    <trd>selle</trd>
  </trdgrp>
  <trd lng="nl">zadel</trd>
  <trd lng="ru">&c_s;&c_je;&c_d;&c_l;&c_o;</trd>
</drv>

<drv mrk="sel.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Meti <tld/>on sur:
      <ekz>
        <tld/>i sian azenon<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">satteln</trd>
  <trd lng="en">saddle</trd>
  <trdgrp lng="fr">
    <trd>seller</trd>
  </trdgrp>
  <trd lng="nl">zadelen</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_d;&c_l;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_s;&c_je;&c_d;&c_l;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
