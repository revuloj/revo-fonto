<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pip</rad>/o
</kap>
<drv mrk="pip.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Tubo fini&gcirc;anta per forneto, en kiun oni metas tabakon, kiun oni
      bruligas por enspiri &gcirc;ian fumon:
      <ekz>
        fumi du <tld/>ojn potage.
      </ekz>
    </dif>
  </snc>             
  <trdgrp lng="be">
    <trd>&c_l;&c_ju;&c_l;&c_mol;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_ib;&c_p;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>korn-butun</trd>,
    <trd>korn<klr tip="ind"> (-butun)</klr></trd>
  </trdgrp>
  <trd lng="ca">pipa</trd>
  <trd lng="de">Pfeife</trd>
  <trd lng="en">pipe</trd>
  <trd lng="es">pipa</trd>
  <trd lng="fr">pipe</trd>
  <trd lng="hu">pipa</trd>
  <trd lng="nl">pijp</trd>
  <trd lng="ru">&c_t;&c_r;&c_u;&c_b;&c_k;&c_a; <klr tip="amb">(&c_k;&c_u;&c_r;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_ja;)</klr></trd>
  <trd lng="sv">pipa</trd>
</drv>
</art>
