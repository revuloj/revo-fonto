<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>gist</rad>/o</kap>
<drv mrk="gist.0o">
  <kap><tld/>o</kap>
  <snc mrk="gist.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Substanco entenanta <tld/>ofungojn, ofte en formo de pulvoro,
      pasto a&ubreve; briketo, uzata por malkompaktigi paston en bakado
      a&ubreve; por <ref cel="fermen.0i">fermentigi</ref> bieron a&ubreve; brandon:
      <ekz>
        la farunon &scirc;utu en pelvon, premu en la mezo kava&jcirc;on, tien metu la
        pecigitan <tld/>on
        <fnt><lok><url
        ref="http://www.uni-leipzig.de/esperanto/texte/bakado.html#4"
        >Germana Potkuko</url></lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="gist.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      Ajna el diversaj unu&ccirc;elaj fungospecoj kiuj plejparte reprodukti&gcirc;as
      nesekse per bur&gcirc;onado a&ubreve; divido:
      <ekz>
        nuntempe multaj sake-farejoj uzas sake-<tld/>ojn vendatajn
        por sake-farado, sed ankora&ubreve; ekzistas malmultaj, kiuj
        faras <ctl>Moto</ctl>n utiligante naturan <tld/>on ekzistantan en
        la
        aero
        <fnt><lok><url
        ref="http://www5b.biglobe.ne.jp/~izmibasi/esp/esphomel3.html#3"
        >Sake-farejo, Izumibashi</url></lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_o;&c_zh;&c_d;&c_zh;&c_y;</trd>
  <trd lng="de">Hefe</trd>
  <trd lng="en">yeast</trd>
  <trd lng="hu">s&ouml;r&eacute;leszt&odblac;</trd>
  <trd lng="nl">gist</trd>
  <trd lng="pt">fermento</trd>
  <trd lng="ru">&c_d;&c_r;&c_o;&c_zh;&c_zh;&c_i;</trd>
</drv>

<drv mrk="gist.bier0o">
  <kap>bier<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="G"/>o de la speco kiun oni trovas en bierfe&ccirc;o,
      uzata en fermenta&jcirc;oj por pan- kaj bier-farado.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_w;&c_n;&c_y;&c_ja; &c_d;&c_r;&c_o;&c_zh;&c_d;&c_zh;&c_y;</trd>
  <trd lng="de">Bierhefe</trd>
  <trd lng="fr">levure de bi&egrave;re</trd>
  <trd lng="nl">biergist</trd>
  <trd lng="pt">levedura de cerveja</trd>
  <trd lng="ru">&c_p;&c_i;&c_v;&c_n;&c_y;&c_je; &c_d;&c_r;&c_o;&c_zh;&c_zh;&c_i;</trd>
</drv>
</art>
