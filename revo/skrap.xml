<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: skrap.xml,v 1.31 2020/11/27 01:08:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>skrap</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="skrap.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="skrap.0i.grati">
    <dif>
      Forigi partetojn el la supra&jcirc;o de io,
      <ref tip="super" cel="grat.0i">grati</ref>
      la&ubreve;lar&gcirc;e per ilo kun akra e&gcirc;o:
      <ekz>
        li prenis potpecon por <tld/>adi sin per &gcirc;i
        sidante meze de cindro
        <fnt><bib>MT</bib>, <lok>Ijob 2:8</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rabot.0i">raboti</ref>,
      <ref cel="rasp.0i">raspi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>schaben</trd>,
      <trd>kratzen</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kapar</trd>,
      <trd>levakar</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>skroba&cacute;</trd>,
      <trd>oskrobywa&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_k;&c_r;&c_je;&c_s;&c_t;&c_i;</trd>,
      <trd>&c_s;&c_k;&c_o;&c_b;&c_l;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="sv">skrapa</trd>
    <adm>
      aldoni pli da ekz-oj,
      al&gcirc;ustigi referencotipon al raspi, raboti? <aut>wd</aut>
    </adm>
  </snc>
  <snc mrk="skrap.0i.SON">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <tld lit="S"/>ante estigi malagrablajn sonojn:
      <ekz>
        <tld/>i violonon <klr>(mallerte ludi &gcirc;in)</klr>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>skroba&cacute;</trd>,
      <trd>skrzypie&cacute;</trd>,
      <trd>rz&eogonek;poli&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_i;&c_r;&c_k;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_k;&c_r;&c_je;&c_s;&c_t;&c_i;</trd>,
      <trd>&c_c;&c_a;&c_r;&c_a;&c_p;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kratzen</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_r;&c_e;&c_b;&c_c;&c_ib;</trd>,
    <trd>&c_d;&c_r;&c_a;&c_p;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_ch;&c_y;&c_r;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>se&scaron;krabat</trd>,
    <trd>se&scaron;krabovat</trd>,
    <trd>se&scaron;kr&aacute;bat</trd>,
    <trd>se&scaron;kr&aacute;bnout</trd>,
    <trd>strouhat</trd>,
    <trd>&scaron;krabat</trd>
  </trdgrp>
  <trd lng="en">scrape</trd>
  <trdgrp lng="es">
    <trd>rascar</trd>,
    <trd>raspar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gratter</trd>,
    <trd>racler</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schrapen</trd>,
    <trd>krabben</trd>,
    <trd>krassen</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>o&scaron;kraba&tcaron; <klr>(zemiaky, zeleninu)</klr></trd>,
    <trd>str&uacute;ha&tcaron;</trd>,
    <trd>&scaron;kraba&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="skrap.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="skrap.0ilo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Ilo kun akra e&gcirc;o destinita por <tld/>ado:
      <ekz>
        la ne&gcirc;ulo havis en la korpo fornan <tld/>ilon
        <fnt><bib>Fab4</bib>, <lok>Ne&gcirc;ulo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>&scaron;krabadlo</trd>,
    <trd>&scaron;krabka</trd>
  </trdgrp>
  <trd lng="de">Schaber</trd>
  <trdgrp lng="es">
     <trd>rascador</trd>,
     <trd>raspador</trd>
   </trdgrp>
  <trdgrp lng="fr">
    <trd>grattoir</trd>,
    <trd>racloir</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kapar&oacute;</trd>,
    <trd>vakar&oacute;</trd>
  </trdgrp>
  <trdgrp lng="pl">
      <trd>skrobak</trd>,
      <trd>skrobaczka</trd>
    </trdgrp>
  <trd lng="ru">&c_s;&c_k;&c_r;&c_je;&c_b;&c_o;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>str&uacute;hadlo</trd>,
    <trd>&scaron;krabadlo</trd>
  </trdgrp>
  <trd lng="sv">skrapa<klr tip="amb"> (subst.)</klr></trd>
</drv>

<drv mrk="skrap.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="skrap.cxirkaux0i.KOMUNE">
    <dif>
      <tld lit="S"/>i &ccirc;e la &ccirc;irka&ubreve;o;
      <tld/>i &ccirc;iuflanke:
      <ekz>
        oni elrompis la <klr>[malbonajn]</klr> &scirc;tonojn kaj
        &ccirc;irka&ubreve;<tld/>is la domon kaj kalkis
        <fnt><bib>MT</bib>, <lok>&Lev; 14:43</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es"><ind>raspar</ind> alrededor</trd>
  <trdgrp lng="hu">
    <trd>k&ouml;rbekapar</trd>,
    <trd>k&ouml;rbevakar</trd>
  </trdgrp>
</drv>

<drv mrk="skrap.de0i">
  <kap>de<tld/>i,
  <var><kap>for<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="skrap.de0i.KOMUNE">
    <dif>
      Forigi <tld/>ante:
      <ekz>
        la domon oni pri<tld/>u interne &ccirc;irka&ubreve;e,
        kaj la kalka&jcirc;on, kiun oni de<tld/>is, oni &jcirc;etu
        ekster la urbon
        <fnt><bib>MT</bib>, <lok>&Lev; 14:41</lok></fnt>;
      </ekz>
      <ekz>
        ili elprenis siajn po&scirc;tran&ccirc;ilojn kaj komencis for<tld/>i la ne&gcirc;on de siaj vestoj
        <fnt>
          <aut>H. Laxness, trad. B. Ragnarsson</aut>
          <vrk>Sendependaj homoj</vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">od&scaron;krabat</trd>
  <trdgrp lng="de">
    <trd>abschaben</trd>,
    <trd>abkratzen</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>quitar raspando</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&ocirc;ter<klr tip="ind"> (en raclant)</klr></trd>,
    <trd>racler<klr tip="ind"> (pour &ocirc;ter)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>levakar</trd>,
    <trd>lekapar</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zeskroba&cacute;</trd>,
    <trd>obskroba&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_s;&c_k;&c_r;&c_je;&c_b;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_s;&c_k;&c_r;&c_je;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_t;&c_s;&c_k;&c_o;&c_b;&c_l;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zo&scaron;kriaba&tcaron;</trd>
  <trd lng="sv">skrapa bort</trd>
</drv>

<drv mrk="skrap.cxiel0anto">
  <kap>&ccirc;iel<tld/>anto,
  <var><kap>nub<tld/>anto</kap></var></kap>
  <snc mrk="skrap.cxiel0anto.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Altega <ref tip="super" cel="dom.0o.konstruajxo">domo</ref>:
      <ekz>
        &ccirc;erande de la horizonto vicas jam blankaj nub<tld/>antoj
        <fnt><aut>I. Nemere</aut>, <vrk>Sur Kampo Granita</vrk>, 
        <lok>1983</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_m;&c_a;&c_r;&c_a;&c_ch;&c_o;&c_s;</trd>
  <trd lng="de">Wolkenkratzer</trd>
  <trd lng="fr">gratte-ciel</trd>
  <trd lng="hu">felh&odblac;karcol&oacute;</trd>
  <trd lng="nl">wolkenkrabber</trd>
  <trd lng="pl">drapacz chmur</trd>
  <trd lng="ru">&c_n;&c_je;&c_b;&c_o;&c_s;&c_k;&c_r;&c_jo;&c_b;</trd>
  <trd lng="sv">skyskrapa</trd>
</drv>

<drv mrk="skrap.dent0i">
  <kap>dent<tld/>i,
  <var><kap><tld/>man&gcirc;i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="skrap.dent0i.KOMUNE">
    <dif>
      Man&gcirc;i &ccirc;irka&ubreve;<tld/>ante per la dentoj;
      <ref tip="sin" cel="rongx.0i">ron&gcirc;i</ref>.
    </dif>
  </snc>
  <trd lng="de">nagen</trd>
  <trd lng="fr">ronger</trd>
  <trd lng="hu">r&aacute;g</trd>
  <trd lng="pl">obgryza&cacute;</trd>
  <trd lng="ru">&c_g;&c_r;&c_y;&c_z;&c_t;&c_mol;</trd>
  <trd lng="sv">gnaga</trd>
</drv>

<drv mrk="skrap.0gumo">
  <kap><tld/>gumo</kap>
  <snc mrk="skrap.0gumo.TIP">
    <dif>
      Ilo el gumo a&ubreve; simila materialo por
      korekti desegna&jcirc;on, faritan per grafita krajono,
      for<tld/>ante iujn strekojn:
      <ekz>
        en britaj butikoj mi vidis dinosa&ubreve;rajn afi&scirc;ojn,
        librojn, balonetojn, puzlojn, notlibretojn, stampilojn,
        <tld/>gumpecojn, &ccirc;efe por niaj infanoj
        <fnt>
          <aut>M. Boulton</aut>
          <vrk>Dinomanio</vrk>
          <bib>Monato</bib>,
          <lok>1994:02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">guma na gumov&aacute;n&iacute;</trd>
  <trd lng="de">Radiergummi</trd>
  <trd lng="fr">gomme<klr tip="ind"> (pour effacer)</klr></trd>
  <trdgrp lng="hu">
    <trd>rad&iacute;rgumi</trd>,
    <trd>t&ouml;rl&odblac;gumi</trd>
  </trdgrp>
  <trd lng="pl">gumka<klr> do gumowania</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_s;&c_t;&c_i;&c_k;</trd>,
    <trd>&c_s;&c_t;&c_jo;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_i;&c_r;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_r;&c_je;&c_z;&c_i;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">guma <klr>(na zotieranie p&iacute;sma)</klr></trd>
  <trdgrp lng="sv">
    <trd>radergummi</trd>,
    <trd>suddgummi</trd>
  </trdgrp>
</drv>
</art>
