  </trdgrp>
  <trd lng="ru">&c_s;&c_k;&c_r;&c_je;&c_b;&c_o;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>str&uacute;hadlo</trd>,
    <trd>&scaron;krabadlo</trd>
  </trdgrp>
  <trd lng="sv">skrapa<klr tip="amb"> (subst.)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_k;&c_r;&c_je;&c_b;&c_a;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_je;&c_b;&c_o;&c_k;</trd>,
    <trd>&c_sh;&c_k;&c_r;&c_je;&c_b;&c_o;&c_k;</trd>,
    <trd>&c_sh;&c_a;&c_b;&c_je;&c_r;</trd>
