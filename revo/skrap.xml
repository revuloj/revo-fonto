<drv mrk="skrap.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="skrap.0i.grati">
    <dif>
      Forigi partetojn el la supra&jcirc;o de io,
      <ref tip="super" cel="grat.0i">grati</ref>
      la&ubreve;lar&gcirc;e per ilo kun akra e&gcirc;o:
      <ekz>
        li prenis potpecon por <tld/>adi sin per &gcirc;i
        sidante meze de cindro
        <fnt><bib>MT</bib> <lok>Ijob 2:8</lok></fnt>;
      </ekz>
      <ekz>
        amasege sidis sur <klr>[la &ccirc;evaloj]</klr> la mu&scirc;oj kaj
        <klr>[&#8230;]</klr> la veturigisto malsupreniris kaj <tld/>is la bestojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> aktoro <klr>[&#8230;]</klr> rapide reeniras en sian lo&gcirc;ion, por
        <tld/>i la falsan koloron almetitan al lia viza&gcirc;o
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi povos pro l' demando vian kapon longe <tld/>i
        <klr>(<ref cel="pens.pri0i">pripensante</ref>)</klr>
        <fnt>
          <bib>LSB</bib>
        </fnt>;
      </ekz>
      <ekz>
        li el<tld/>is oran moneron el la monujo
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> <tld/>is per la akra&jcirc;o sur la
        sesterco la signon de kruco
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la bestio <klr>[&#8230;]</klr> mu&gcirc;as kaj hufo<tld/>as
        <klr>(kp <ref cel="grat.0i">tergrati</ref>)</klr>
        &gcirc;uste anta&ubreve; ol ataki tiujn
        provoke defiantaj &gcirc;in
        <fnt>
          <bib>Monato</bib>
          <aut>Eduardo Novembro</aut>
          <vrk><url ref="&Monato;/2014/011318.php">Ta&ubreve;ro??? Festo!!!</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frot.0i">froti</ref>,
      <ref cel="rabot.0i">raboti</ref>,
      <ref cel="rasp.0i">raspi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>schaben</trd>,
      <trd>kratzen</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kapar</trd>,
      <trd>levakar</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>skroba&cacute;</trd>,
      <trd>oskrobywa&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_k;&c_r;&c_je;&c_s;&c_t;&c_i;</trd>,
      <trd>&c_s;&c_k;&c_o;&c_b;&c_l;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="sv">skrapa</trd>
    <adm>
      aldoni pli da ekz-oj,
      al&gcirc;ustigi referencotipon al raspi, raboti? <aut>wd</aut>
    </adm>
  </snc>
  <snc mrk="skrap.0i.SON">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <tld lit="S"/>ante estigi malagrablajn sonojn:
      <ekz>
        <tld/>i violonon <klr>(mallerte ludi &gcirc;in)</klr>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>skroba&cacute;</trd>,
      <trd>skrzypie&cacute;</trd>,
      <trd>rz&eogonek;poli&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_i;&c_r;&c_k;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_k;&c_r;&c_je;&c_s;&c_t;&c_i;</trd>,
      <trd>&c_c;&c_a;&c_r;&c_a;&c_p;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kratzen</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_r;&c_e;&c_b;&c_c;&c_ib;</trd>,
    <trd>&c_d;&c_r;&c_a;&c_p;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_ch;&c_y;&c_r;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>se&scaron;krabat</trd>,
    <trd>se&scaron;krabovat</trd>,
    <trd>se&scaron;kr&aacute;bat</trd>,
    <trd>se&scaron;kr&aacute;bnout</trd>,
    <trd>strouhat</trd>,
    <trd>&scaron;krabat</trd>
  </trdgrp>
  <trd lng="en">scrape</trd>
  <trdgrp lng="es">
    <trd>rascar</trd>,
    <trd>raspar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gratter</trd>,
    <trd>racler</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#21066;&#12426;&#21462;&#12427;  <pr>&#12369;&#12378;&#12426;&#12392;&#12427;</pr></trd>,
    <trd>&#12363;&#12365;&#33853;&#12392;&#12377;  <pr>&#12363;&#12365;&#12362;&#12392;&#12377;</pr></trd>,
    <trd>&#12371;&#12381;&#12370;&#12427;</trd>,
    <trd>&#12365;&#12356;&#12365;&#12356;&#40180;&#12425;&#12377;  <pr>&#12365;&#12356;&#12365;&#12356;&#12394;&#12425;&#12377;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schrapen</trd>,
    <trd>krabben</trd>,
    <trd>krassen</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>o&scaron;kraba&tcaron; <klr>(zemiaky, zeleninu)</klr></trd>,
    <trd>str&uacute;ha&tcaron;</trd>,
    <trd>&scaron;kraba&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_je;&c_r;&c_l;&c_i;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_c;&c_i;&c_g;&c_i;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_i;&c_l;&c_ja;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#25814; <pr>g&ecaron;</pr></trd>
</drv>

<drv mrk="skrap.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="skrap.0ilo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Ilo kun akra e&gcirc;o destinita por <tld/>ado:
      <ekz>
        la ne&gcirc;ulo havis en la korpo fornan <tld/>ilon
        <fnt><bib>Fab4</bib> <lok>Ne&gcirc;ulo</lok></fnt>;
      </ekz>
      <ekz>
        dento<tld/>ilo
        <klr>(vd <ref cel="pingl.dent0o">dentpinglo</ref>)</klr>
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        iliaj laborinstrumentoj estas motorsegiloj, hakiloj, marteloj, &ccirc;iziloj,
        <tld/>iloj
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2006/009096.php">Glaciskulptado</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kling.0o">klingo</ref>,
      <ref cel="rabot.0ilo">rabotilo</ref>,
      <ref cel="rasp.0ilo">raspilo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_r;&c_a;&c_b;&c_a;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_e;&c_b;&c_l;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>&scaron;krabadlo</trd>,
    <trd>&scaron;krabka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schaber</trd>,
    <trd>Kratzer <klr>(Schaber)</klr></trd>,
    <trd>Striegel</trd>,
    <trd>Abstreicher</trd>
