<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>skrap</rad>/i
</kap>
<drv mrk="skrap.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Forigi partetojn el la supra&jcirc;o de io, gratante &gcirc;in:
      <ekz>
        <tld/>i svisan froma&gcirc;on.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="rasp.0i">raspi</ref>,
      <ref cel="rabot.0i">raboti</ref>.
    </refgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_k;&c_r;&c_je;&c_s;&c_t;&c_i;</trd>,
      <trd>&c_s;&c_k;&c_o;&c_b;&c_l;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Purigi kamenon gratante la fulgon.
    </dif>
    <refgrp tip="sin">
      <ref cel="rast.0i">rasti</ref>,
      <ref cel="frot.0i">froti</ref>.
    </refgrp>
    <trd lng="ru">&c_s;&c_k;&c_o;&c_b;&c_l;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Estigi malagrablajn sonojn: frotante ion:
      <ekz>
        li <tld/>as de ni la orelojn.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_i;&c_r;&c_k;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_k;&c_r;&c_je;&c_s;&c_t;&c_i;</trd>,
      <trd>&c_c;&c_a;&c_r;&c_a;&c_p;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Mallerte ludi ar&ccirc;an muzikinstrumenton:
      <ekz>
        <tld/>i violonon.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="grat.0i">grati</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>kratzen</trd>,
    <trd>schaben</trd>
  </trdgrp>
  <trd lng="en">scrape</trd>
  <trdgrp lng="nl">
    <trd>schrapen</trd>,
    <trd>krabben</trd>,
    <trd>krassen</trd>
  </trdgrp>
</drv>

<drv mrk="skrap.0mangxident0i">
  <kap><tld/>man&gcirc;i, dent<tld/>i</kap>
  <snc>
    <dif>
      Man&gcirc;i ion &ccirc;irka&ubreve;<tld/>ante per la dentoj.
    </dif>
  </snc>
</drv>

<drv mrk="skrap.cxiel0antonub0ajxo">
  <kap>&ccirc;iel<tld/>anto, nub<tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Altega domo.
    </dif>
  </snc>
  <trd lng="de">Wolkenkratzer</trd>
  <trd lng="nl">wolkenkrabber</trd>
  <trd lng="ru">&c_n;&c_je;&c_b;&c_o;&c_s;&c_k;&c_r;&c_jo;&c_b;</trd>
</drv>
</art>
