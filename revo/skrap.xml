  <trdgrp lng="es">
     <trd>rascador</trd>,
     <trd>raspador</trd>
   </trdgrp>
  <trdgrp lng="fr">
    <trd>grattoir</trd>,
    <trd>racloir</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kapar&oacute;</trd>,
    <trd>vakar&oacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12365;&#12373;&#12370;</trd>,
    <trd>&#12473;&#12463;&#12524;&#12540;&#12497;&#12540;</trd>
  </trdgrp>
  <trdgrp lng="pl">
