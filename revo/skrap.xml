<drv mrk="skrap.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="skrap.cxirkaux0i.KOMUNE">
    <dif>
      <tld lit="S"/>i &ccirc;e la &ccirc;irka&ubreve;o;
      <tld/>i &ccirc;iuflanke:
      <ekz>
        oni elrompis la <klr>[malbonajn]</klr> &scirc;tonojn kaj
        &ccirc;irka&ubreve;<tld/>is la domon kaj kalkis
        <fnt><bib>MT</bib> <lok>&Lev; 14:43</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_s;&c_k;&c_r;&c_a;&c_b;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_s;&c_k;&c_r;&c_e;&c_b;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="es"><ind>raspar</ind> alrededor</trd>
  <trdgrp lng="hu">
    <trd>k&ouml;rbekapar</trd>,
    <trd>k&ouml;rbevakar</trd>
  </trdgrp>
  <trd lng="pl">oskroba&cacute;</trd>
</drv>

<drv mrk="skrap.de0i">
  <kap>de<tld/>i,
  <var><kap>for<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="skrap.de0i.KOMUNE">
    <dif>
      Forigi <tld/>ante:
      <ekz>
        la domon oni pri<tld/>u interne &ccirc;irka&ubreve;e,
        kaj la kalka&jcirc;on, kiun oni de<tld/>is, oni &jcirc;etu
        ekster la urbon
        <fnt><bib>MT</bib> <lok>&Lev; 14:41</lok></fnt>;
      </ekz>
      <ekz>
        ili elprenis siajn po&scirc;tran&ccirc;ilojn kaj komencis for<tld/>i la ne&gcirc;on de siaj vestoj
        <fnt>
          <aut>H. Laxness, trad. B. Ragnarsson</aut>
          <vrk>Sendependaj homoj</vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_s;&c_k;&c_r;&c_a;&c_b;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_s;&c_k;&c_r;&c_e;&c_b;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">od&scaron;krabat</trd>
  <trdgrp lng="de">
    <trd>abschaben</trd>,
    <trd>abkratzen</trd>,
    <trd>abstreichen</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>quitar raspando</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&ocirc;ter<klr tip="ind"> (en raclant)</klr></trd>,
    <trd>racler<klr tip="ind"> (pour &ocirc;ter)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>levakar</trd>,
    <trd>lekapar</trd>
  </trdgrp>
  <trd lng="ja">&#21066;&#12426;&#21462;&#12387;&#12390;&#12375;&#12414;&#12358;
  <pr>&#12369;&#12378;&#12426;&#12392;&#12387;&#12390;&#12375;&#12414;&#12358;</pr></trd>
  <trdgrp lng="pl">
    <trd>zeskroba&cacute;</trd>,
    <trd>obskroba&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_s;&c_k;&c_r;&c_je;&c_b;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_s;&c_k;&c_r;&c_je;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_t;&c_s;&c_k;&c_o;&c_b;&c_l;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zo&scaron;kriaba&tcaron;</trd>
  <trd lng="sv">skrapa bort</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_ib;&c_s;&c_k;&c_o;&c_b;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_d;&c_r;&c_ja;&c_p;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_ib;&c_sh;&c_k;&c_r;&c_ja;&c_b;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21038;&#25481; <pr>gu&amacron;di&agrave;o</pr></trd>,
    <trd>&#21038;&#21435; <pr>gu&amacron;q&ugrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="skrap.el0i">
  <kap>
    el<tld/>i, <var><kap>kun<tld/>i</kap></var>
  </kap>
  <snc mrk="skrap.el0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Pene <ref tip="super" cel="kolekt.0i.kunmeti">kolekti</ref> valora&jcirc;on,
      bezonata&jcirc;on:
      <ekz>
        kun &scirc;ia helpo ni kun<tld/>is etan kapitalon
        <fnt>
          <bib>KKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        la tridekjara, sukcesplena kaj talenta industriisto, vane klopodis kun<tld/>i
        sian memon el la ruinoj de la okaza&jcirc;oj
        <fnt>
          <bib>KKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        li boris la fundon de la fosa&jcirc;o per la bastongumo, kvaza&ubreve; <klr>[&#8230;]</klr> tie
        estas io speciale valora, kiun li povas el<tld/>i
        <fnt>
          <bib>VRA</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_s;&c_k;&c_r;&c_a;&c_b;&c_a;&c_c;&c_mol; <klr>(&c_p;&c_je;&c_r;&c_a;&c_n;.)</klr></trd>
  <trd lng="fr">racler les fonds de tiroir</trd>
  <trd lng="de">zusammenkratzen</trd>
  <trd lng="pl">wyd&lstroke;uba&cacute;</trd>
</drv>

<drv mrk="skrap.cxiel0anto">
  <kap>&ccirc;iel<tld/>anto,
    <var><kap>&ccirc;iel<tld/>ulo</kap></var>,
    <var><kap>nub<tld/>anto</kap></var>,
    <var><kap>nub<tld/>ulo</kap></var>
  </kap>
  <snc mrk="skrap.cxiel0anto.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="dom.alt0o">Altdomo, turdomo</ref>:
      <ekz>
        estis granda haveno, &gcirc;i havis altajn nub<tld/>ulojn kaj ludomojn, kies fenestroj
        &ccirc;iuj estis lumigataj
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 48</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;erande de la horizonto vicas jam blankaj nub<tld/>antoj
        <fnt><aut>I. Nemere</aut>, <vrk>Sur Kampo Granita</vrk>,
        <lok>1983</lok></fnt>;
      </ekz>
      <ekz>
        japanoj <klr>[&#8230;]</klr> pro multjarcentaj spertoj scias konstrui e&ccirc;
        nub<tld/>ulojn tiel, ke ili rezistas al tertremo
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2005/008775.php">Humiligita Usono</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;irka&ubreve; la distrikto <nom>Hongkou</nom> estas pli kaj pli
        multaj modernaj &ccirc;iel<tld/>antoj
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="&Monato;/2015/011566.php">Judaj rifu&gcirc;intoj en &Scirc;anhajo</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni dronas ie inter urbaj &ccirc;iel<tld/>uloj dum malfrua krepusko
        <fnt>
          <bib>LOdE</bib>
          <lok> 2000,  No 5 (67)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_m;&c_a;&c_r;&c_a;&c_ch;&c_o;&c_s;</trd>
  <trd lng="de">Wolkenkratzer</trd>
  <trd lng="fr">gratte-ciel</trd>
  <trd lng="hu">felh&odblac;karcol&oacute;</trd>
  <trd lng="ja">&#25705;&#22825;&#27004;  <pr>&#12414;&#12390;&#12435;&#12429;&#12358;</pr></trd>
  <trd lng="nl">wolkenkrabber</trd>
  <trdgrp lng="pl">
    <trd>drapacz chmur</trd>,
    <trd>wie&zdot;owiec</trd>
  </trdgrp>
  <trd lng="ro">zg&acirc;rie-nori</trd>
  <trd lng="ru">&c_n;&c_je;&c_b;&c_o;&c_s;&c_k;&c_r;&c_jo;&c_b;</trd>
  <trd lng="sv">skyskrapa</trd>
  <trd lng="uk">&c_h;&c_m;&c_a;&c_r;&c_o;&c_ch;&c_o;&c_s;</trd>
  <trdgrp lng="zh">
    <trd>&#25705;&#22825;&#22823;&#27004; <pr>m&oacute;ti&amacron;nd&agrave;l&oacute;u</pr></trd>,
    <trd>&#25705;&#22825;&#22823;&#27155; <pr>m&oacute;ti&amacron;nd&agrave;l&oacute;u</pr></trd>,
    <trd>&#25705;&#22825;&#22823;&#21414; <pr>m&oacute;ti&amacron;nd&agrave;sh&agrave;</pr></trd>,
    <trd>&#25705;&#22825;&#22823;&#24264; <pr>m&oacute;ti&amacron;nd&agrave;sh&agrave;</pr></trd>,
    <trd>&#25705;&#22825;&#27004; <pr>m&oacute;ti&amacron;nl&oacute;u</pr></trd>,
    <trd>&#25705;&#22825;&#27155; <pr>m&oacute;ti&amacron;nl&oacute;u</pr></trd>,
    <trd>&#36229;&#39640;&#23618;&#22823;&#27004; <pr>ch&amacron;og&amacron;oc&eacute;ngd&agrave;l&oacute;u</pr></trd>,
    <trd>&#36229;&#39640;&#23652;&#22823;&#27155; <pr>ch&amacron;og&amacron;oc&eacute;ngd&agrave;l&oacute;u</pr></trd>
  </trdgrp>
</drv>

<drv mrk="skrap.dent0i">
  <kap>dent<tld/>i,
  <var><kap><tld/>man&gcirc;i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
