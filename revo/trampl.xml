<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: trampl.xml,v 1.4 2019/10/17 20:10:26 revo Exp $">
<kap>
  <rad>trampl</rad>/i <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="trampl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="trampl.0i.KOMUNE">
    <dif>
      <ref tip="dif" cel="tret.0i.KOMUNE">Treti</ref>:
      <ekz>
        li <klr>[&#8230;]</klr> <tld/>is en babu&scirc;oj kaj klakis per pantofloj
        <fnt><bib>RabistFab</bib></fnt>;
      </ekz>
      <ekz>
        estis pasejo inter malaltaj bordoj multe <tld/>ita pro transiro 
        de &ccirc;evaloj
        <fnt>
          <aut>J. R. R. Tolkien, trad. W. Auld</aut>,
          <vrk>La Mastro de l' Ringoj, 2, La du turegoj</vrk>,
          <lok>1996</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="stamf.0i">stamfi</ref>
  </snc>
  <trd lng="cs">odrazit se souno&zcaron;</trd>
  <trd lng="de">trampeln</trd>
  <trd lng="fr">pi&eacute;tiner</trd>
  <trd lng="sk">odrazi&tcaron; sa</trd>
</drv>
</art>

