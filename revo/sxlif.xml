<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&scirc;lif</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sxlif.0i">
  <kap><tld/>i</kap>
  <snc mrk="sxlif.0i.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="uz.el0i.foruzi">Eluze</ref>
      tran&ccirc;i, fasoni a&ubreve; glatigi malmolan a&jcirc;on
      per la rapida movi&gcirc;o de multaj tran&ccirc;aj e&gcirc;oj:
      <ekz>
        en sukcesa <tld/>operacio la prilaborata peco
        restas la&ubreve;eble plej malvarma
        <fnt>
          <aut>Griza Leono</aut>,
          <vrk><url
          ref="https://groups.google.com/d/msg/la-bona-lingvo---redaktantoj/rMQmXJimC5U/323FOFRw3PQJ">
          shlifi 605039</url></vrk>,
          <lok>La bona lingvo, 2011-02-05</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="abrazi.0i">abrazii</ref>,
      <ref cel="erozi.0i">erozii</ref>,
      <ref cel="triv.0i.KOMUNE">trivi</ref>
    </refgrp>
  </snc>
  <trd lng="de">schleifen</trd>
  <trd lng="en">grind</trd>
  <trd lng="fr">meuler</trd>
</drv>

<drv mrk="sxlif.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="sxlif.0ilo.MAS">
    <uzo tip="fak">MAS</uzo>
    <dif>
      Aparato por <tld/>i, provizita
      je multaj malgrandaj e&gcirc;oj.
    </dif>
  </snc>
  <trd lng="fr">meule</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>