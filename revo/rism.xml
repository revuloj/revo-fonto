<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rism.xml,v 1.16 2022/11/30 20:30:06 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>rism</rad>/o
</kap>
<drv mrk="rism.0o">
  <kap><tld/>o</kap>
  <snc mrk="rism.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref tip="lst" lst="voko:kvantoj" cel="ar.0o.KOMUNE">Aro</ref> da 500 paperfolioj:
      <ekz>
        mi mendas de vi, por provo, cent <tld/>ojn da papero konforma je
        specimeno n-ro 1
        <fnt>
          <bib>FK</bib>, <lok>5, el la vivo kaj sciencoj, korespondado
          komerca, (de L. de Beaufront)</lok>
        </fnt>.
      </ekz>
      <ekz>
        mia <klr>[&#8230;]</klr> edzino transdonis al mi <tld/>on da artikoloj,
        fiktiva&jcirc;oj kaj eseoj por Literatura Verniso
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 8</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">rys pap&iacute;ru</trd>
  <trd lng="de">Ries</trd>
  <trd lng="en">ream</trd>
  <trd lng="fr">ramette</trd>
  <trd lng="hu">rizsma <klr tip="amb">(ezer &iacute;v)</klr></trd>
  <trd lng="nl">riem<klr tip="ind"> (papier)</klr></trd>
  <trd lng="pl">ryza</trd>
  <trd lng="pt">resma</trd>
  <trd lng="ro">top de h&acirc;rtie</trd>
  <trd lng="ru">&c_s;&c_t;&c_o;&c_p;&c_a; <klr tip="amb">(&c_m;&c_je;&c_r;&c_a; &c_b;&c_u;&c_m;&c_a;&c_g;&c_i;)</klr></trd>
  <trd lng="sk">rys papieru</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_o;&c_p;&c_a; (&c_p;&c_a;&c_p;&c_je;&c_r;&c_u;)</trd>,
    <trd>&c_s;&c_t;&c_a;&c_n;&c_d;&c_a;&c_r;&c_t;&c_n;&c_a; &c_p;&c_a;&c_ch;&c_k;&c_a; &c_p;&c_a;&c_p;&c_je;&c_r;&c_u; (500 &c_l;&c_i;&c_s;&c_t;&c_ib;&c_v;)</trd>
  </trdgrp>
</drv>
</art>
