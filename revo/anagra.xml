    <trd>&#x5D0;&#x5B2;&#x5E0;&#x5B7;&#x5D2;&#x5B0;&#x5E8;&#x5B7;&#x5DE;&#x5B8;&#x5D4;</trd>,
    <trd>&#x5D4;&#x5D7;&#x5DC;&#x5E4;&#x5EA; &#x5E1;&#x5D3;&#x5E8; &#x5D0;&#x5D5;&#x5EA;&#x5D9;&#x5D5;&#x5EA;</trd>
  </trdgrp>
  <trd lng="hu">anagramma</trd>
  <trd lng="nl">anagram</trd>
  <trd lng="pl">anagram</trd>
  <trd lng="ru">&c_a;&c_n;&c_a;&c_g;&c_r;&c_a;&c_m;&c_m;&c_a;</trd>
  <trd lng="sk">anagram</trd>
  <trd lng="uk">&#x430;&#x43D;&#x430;&#x433;&#x440;&#x430;&#x43C;&#x430;</trd>
