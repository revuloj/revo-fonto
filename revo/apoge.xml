<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>apoge</rad>/o
</kap>
<drv mrk="apoge.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Punkto de la orbito de astro, &ccirc;e kiu &gcirc;i estas plej malproksima
      de la tero.
    </dif>
    <refgrp tip="sin">
      <ref cel="perige.0o">perigeo</ref>,
      <ref cel="zenit.0o">zenito</ref>,
      <ref cel="kulmin.0o">kulmino</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Apog&auml;um</trd>,
      <trd>Erdferne</trd>
    </trdgrp>
    <trd lng="ru">&c_a;&c_p;&c_o;&c_g;&c_je;&c_j;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plej supera, plej alta grado; egeco:
      <ekz>
        esti &ccirc;e la <tld/>o
        de famo, de sukceso.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="ekstre.0eco">ekstremeco</ref>,
      <ref cel="zenit.0o">zenito</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>H&ouml;hepunkt</trd>,
      <trd>Gipfel</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_a;&c_p;&c_o;&c_g;&c_je;&c_j;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_c;&c_v;&c_je;&c_t;</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">apog&eacute;e</trd>
</drv>
</art>
