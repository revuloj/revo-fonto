<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: apoge.xml,v 1.35 2024/09/14 09:28:21 revo Exp $">
<kap>
   <ofc>1</ofc>
   <rad>apoge</rad>/o
   <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="apoge.0o">
   <kap><tld/>o</kap>
   <snc mrk="apoge.0o.AST">
     <uzo tip="fak">AST</uzo>
     <dif>
       Tiu punkto de la orbito de
       ter-&ccirc;irka&ubreve;anta objekto, &ccirc;e kiu
       tiu estas plej malproksima de la tero.
     </dif>
     <ref tip="ant" cel="perige.0o">perigeo</ref>
     <ref tip="super" cel="apsid.0o.AST">apsido</ref>
     <ref tip="vid" cel="zenit.0o">zenito</ref>
     <trdgrp lng="de">
       <trd>erdfernster Punkt</trd>,
       <trd>Erdferne</trd>,
       <trd>Apog&auml;um</trd>
     </trdgrp>
     <trdgrp lng="hu">
       <trd>f&ouml;ldt&aacute;vol</trd>,
       <trd>apogeum</trd>
     </trdgrp>
     <trdgrp lng="pl">
       <trd>apogeum</trd>,
       <trd>kulminacja</trd>
     </trdgrp>
   </snc>
   <snc mrk="apoge.0o.FIG">
     <uzo tip="stl">FIG</uzo>
     <dif>
       <ref tip="dif" cel="kulmin.0o.FIG">Kulmino<sncref/></ref>,
       <ref tip="sin" cel="zenit.0o.FIG">zenito<sncref/></ref>:
       <ekz>
           esti &ccirc;e la <tld/>o
           de famo, de sukceso;
       </ekz>
       <ekz>
          la 13a jarcento signifas la <tld/>on, kaj Tomaso la Akvinano
          (1225-1274) estas la plej konata reprezentanto
          <fnt>
            <aut>Bo Sandelin</aut>
            <vrk><url ref="&Monato;/2001/007279.php">Anta&ubreve;merkantilisma
            ekonomia pensado</url></vrk>
            <bib>Monato</bib>
            <lok>2001/02, p. 16</lok>
          </fnt>;
       </ekz>
       <ekz>
        la <tld/>o de la Brita Imperio
        <fnt>
          <vrk><url ref="&Viki;/Joseph_Conrad">Joseph
          Conrad - Vikipedio</url></vrk>
        </fnt>.
      </ekz>
     </dif>
     <trdgrp lng="de">
       <trd>H&ouml;hepunkt</trd>,
       <trd>Gipfel</trd>
     </trdgrp>
     <trdgrp lng="en">
       <trd>apogee</trd>,
       <trd>zenith</trd>,
       <trd>pinnacle</trd>,
       <trd>acme</trd>,
       <trd>apex</trd>
     </trdgrp>
     <trdgrp lng="hu">
       <trd>cs&uacute;cs</trd>,
       <trd>cs&uacute;cspont</trd>
     </trdgrp>
     <trdgrp lng="pl">
       <trd>apogeum</trd>,
       <trd>kulminacja</trd>,
       <trd>punkt <ind>kulminacji</ind></trd>,
       <trd>punkt <ind>kulminacyjny</ind></trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>&c_a;&c_p;&c_o;&c_g;&c_je;&c_j;</trd>,
       <trd>&c_r;&c_a;&c_s;&c_c;&c_v;&c_je;&c_t;</trd>
     </trdgrp>
     <trd lng="zh">&#21319;&#38480; <pr>sh&emacron;ngxi&#xE0;n</pr></trd>
</snc>
  <trd lng="be">&c_a;&c_p;&c_a;&c_g;&c_je;&c_j;</trd>
  <trd lng="ca">apogeu</trd>
  <trdgrp lng="cs">
    <trd>vrchol <klr>(k&rcaron;ivky)</klr></trd>,
    <trd>vrcholen&iacute;</trd>,
    <trd>vyvrcholen&iacute;</trd>
  </trdgrp>
  <trd lng="es">apog&eacute;o</trd>
  <trd lng="fr">apog&eacute;e</trd>
  <trdgrp lng="he">
    <trd>&#x5E9;&#x5D9;&#x5D0;</trd>,
    <trd>&#x5E4;&#x5E1;&#x5D2;&#x5D4;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#36960;&#22320;&#x70B9;  <pr>&#12360;&#12435;&#12385;&#12390;&#12435;</pr></trd>,
    <trd>&#26368;&#x9AD8;&#x70B9;  <pr>&#12373;&#12356;&#12371;&#12358;&#12390;&#12435;</pr></trd>,
    <trd>&#x7D76;&#38914;  <pr>&#x305C;&#12387;&#12385;&#12423;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="pt">apogeu</trd>
  <trdgrp lng="sk">
    <trd>apogeum</trd>,
    <trd>odzemie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&#x43F;&#x43E;&c_g;&c_je;&c_j;</trd>,
    <trd>&c_r;&#x43E;&c_z;&#x43A;&c_v;&c_ib;&c_t;</trd>
  </trdgrp>
</drv>
</art>
