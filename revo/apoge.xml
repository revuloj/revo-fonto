<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>apoge</rad>/o
</kap>
<drv mrk="apoge.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">AST</uzo>
    <dif>
  Tiu punkto de la orbito de
  ter&ccirc;irkka&ubreve;anta objekto, kiu estas
  plej malproksima al la Tero.
    </dif>
    <ref tip="ant" cel="perige.0o">perigeo</ref>
    <refgrp tip="vid">
      <ref cel="zenit.0o">zenito</ref>,
      <ref cel="kulmin.0o">kulmino</ref>.
    </refgrp>
    <trdgrp lng="hu">
      <trd>f&ouml;ldt&aacute;vol</trd>,
      <trd>apogeum</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plej supera, plej alta grado; egeco:
      <ekz>
        esti &ccirc;e la <tld/>o
        de famo, de sukceso.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="ekstre.0eco">ekstremeco</ref>,
      <ref cel="zenit.0o">zenito</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>H&ouml;hepunkt</trd>,
      <trd>Gipfel</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>cs&uacute;cs</trd>,
      <trd>cs&uacute;cspont</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_a;&c_p;&c_o;&c_g;&c_je;&c_j;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_c;&c_v;&c_je;&c_t;</trd>
    </trdgrp>
  </snc>
  <trd lng="es">apog&eacute;o</trd>
  <trd lng="fr">apog&eacute;e</trd>
  <trd lng="pt">apogeu</trd>
</drv>
</art>
