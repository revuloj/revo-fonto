<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sekvojadendr.xml,v 1.19 2023/08/05 17:26:35 revo Exp $">
<kap>
  <rad>sekvojadendr</rad>/o
</kap>
<drv mrk="sekvojadendr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Giganta arbo
      el <ref tip="lst" cel="konifer.0o" lst="voko:koniferoj">koniferoj</ref>,
      kreskanta en Kalifornio, kies alto atingas &gcirc;is 100 metrojn,
      kaj kiu fari&gcirc;as &gcirc;is 4000 jarojn a&gcirc;a;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="cipres1.0oj">cipresacoj</ref>
      (<trd lng="la">Sequoiadendron</trd>), kun nur unu specio:
      <ekz>
        <nom>General Sherman Tree</nom> (giganta <tld/>o) en Usono
        estas la&ubreve; la volumeno la plej  granda arbo de la mondo havante
        trunkon de 1489&#x202F;m&cubic; anta&ubreve; la <nom>General Grant Tree</nom>
        (&ccirc;iamverda sekvojo, 1357&#x202F;m&cubic;)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=604756">Listo de rimarkeblaj maljunaj
          arboekzempleroj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
     Iom pli alta, sed ne tiom maljuna fari&gcirc;as la
      <ref tip="vid" cel="sekvoj.0o">sekvojo</ref>.
    </rim>
    <bld
    lok="&WCU;/thumb/9/91/Sequoiadendron_giganteum_08145.JPG/360px-Sequoiadendron_giganteum_08145.JPG"
    prm="CC BY 2.5"><tld/>o (Sequoiadendron giganteum)
        <fnt>
          <aut>Walter Siegmund</aut>
          <lok><url
          ref="&WCW;/File:Sequoiadendron_giganteum_08145.JPG">&WCW;/File:Sequoiadendron_giganteum_08145.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="ca">sequoia</trd>
  <trdgrp lng="de">
    <trd>Berg-Mammutbaum</trd>,
    <trd>Riesenmammutbaum</trd>,
    <trd>Wellingtonie</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>redwood</trd>
     <trd>giant redwood</trd>
     <trd>giant sequoia</trd>
     <trd>Sierra redwood</trd>
     <trd>Wellingtonia</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hegyi <ind>mamutfeny&odblac;</ind></trd>,
    <trd>kaliforniai <ind>mamutfeny&odblac;</ind></trd>
  </trdgrp>
  <trd lng="id"><ind>sequoia</ind> raksasa</trd>
  <trdgrp lng="it">
    <trd><ind>sequoia</ind> gigante</trd>,
    <trd>wellingtonia</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>mammoetboom</trd>,
    <trd>seqoiadendron</trd>
  </trdgrp>
  <trd lng="pl">mamutowiec</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_k;&c_v;&c_o;&c_j;&c_ja;&c_d;&c_je;&c_n;&c_d;&c_r;&c_o;&c_n;</trd>,
    <trd>&c_m;&c_a;&c_m;&c_o;&c_n;&c_t;&c_o;&c_v;&c_o; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_l;&c_i;&c_n;&c_g;&c_t;&c_o;&c_n;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_je;&#x43A;&c_v;&#x43E;&#x44F;&c_d;&c_je;&#x43D;&c_d;&c_r;&#x43E;&#x43D;</trd>
</drv>
</art>
