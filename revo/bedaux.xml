<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>beda&ubreve;r</rad>/i</kap>
<drv mrk="bedaux.0i">
       <kap><ofc>*</ofc><tld/>i</kap>
       <gra><vspec>tr</vspec></gra>
       <snc mrk="bedaux.0i.cxagrene">
         <dif>
           &Ccirc;agrene senti la perdon de iu a&ubreve; io:
           <ekz>
             <tld/>i la mortintan patrinon, la forpasintan junecon;
           </ekz>
           <ekz>
             nia <tld/>ata prezidanto.
           </ekz>
         </dif>
         <refgrp tip="vid">
           <ref cel="gxoj.mal0i">mal&gcirc;oji</ref>,
           <ref cel="funebr">funebri</ref>.
         </refgrp>
       </snc>
       <snc mrk="bedaux.0i.malkontente">
         <dif>
           Esti malkontenta pro ia faro a&ubreve; manko:
           <ekz>
             mi <tld/>as mian
             severecon<fnt>Z</fnt>;
           </ekz>
           <ekz>
             <tld/>i la mal&scirc;paritan tempon;
           </ekz>
           <ekz>
             mi <tld/>as, ke mi vin &ccirc;agrenas<fnt>Z</fnt>,
             ke mi ne povos vin viziti.
           </ekz>
         </dif>
         <refgrp tip="vid">
           <ref cel="pent.0i">penti</ref>,
           <ref cel="domagx.0i">doma&gcirc;i</ref>.
         </refgrp>
       </snc>
       <trdgrp lng="be">
         <trd>&c_sh;&c_k;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
         <trd>&c_z;&c_a;&c_s;&c_m;&c_u;&c_ch;&c_a;&c_c;&c_c;&c_a;</trd>
       </trdgrp>
       <trd lng="cs">litovat</trd>
       <trdgrp lng="de">
          <trd>bedauern</trd>,
          <trd>leid tun</trd>
       </trdgrp>
       <trdgrp lng="en">
         <trd>be <ind>sorry</ind></trd>,
         <trd>regret</trd>,
         <trd>miss</trd>
       </trdgrp>
       <trd lng="fr">regretter</trd>
       <trdgrp lng="he">
         <trd>&lamed;<ind>&he;&tsadi;&tet;&ayin;&resh;</ind></trd>,
         <trd>&lamed;<ind>&he;&tav;&het;&resh;&tet;</ind></trd>
       </trdgrp>
       <trd lng="hu">sajn&aacute;l</trd>
       <trd lng="nl">spijt hebben over</trd>
       <trdgrp lng="ru">
         <trd>&c_zh;&c_a;&c_l;&c_je;&c_t;&c_mol;</trd>,
         <trd>&c_s;&c_o;&c_zh;&c_a;&c_l;&c_je;&c_t;&c_mol;</trd>,
         <trd>&c_b;&c_y;&c_t;&c_mol;
&c_o;&c_g;&c_o;&c_r;&c_ch;&c_jo;&c_n;&c_n;&c_y;&c_m;</trd>
       </trdgrp>
</drv>

<drv mrk="bedaux.0o">
       <kap><tld/>o</kap>
       <snc>
         <dif>
           Sento de tiu, kiu <tld/>as.
         </dif>
       </snc>
       <trdgrp lng="be">
         <trd>&c_zh;&c_a;&c_l;&c_mol;</trd>,
         <trd>&c_sh;&c_k;&c_a;&c_d;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
         <trd>&c_z;&c_a;&c_s;&c_m;&c_u;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
       </trdgrp>
       <trd lng="de">Bedauern</trd>
       <trdgrp lng="en">
         <trd>regret</trd>,
         <trd>sorrow</trd>,
         <trd>remorse</trd>
       </trdgrp>
       <trd lng="fr">regret</trd>
       <trdgrp lng="he">
         <trd>&het;&resh;&tet;&he;</trd>,
         <trd>&tsadi;&ayin;&resh;</trd>
       </trdgrp>
       <trd lng="hu">sajn&aacute;lat</trd>
       <trd lng="nl">spijt</trd>
       <trd lng="ru">&c_s;&c_o;&c_zh;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="bedaux.0e">
       <kap><tld/>e</kap>
       <snc>
         <dif>
           Kun <tld/>o, <tld/>ante:
           <ekz>
             mi <tld/>e eksciis lian morton, havis neniun monon.
           </ekz>
         </dif>
       </snc>
       <trd lng="be">&c_z; <ind>&c_zh;&c_a;&c_l;&c_je;&c_m;</ind></trd>
       <trdgrp lng="en">
         <trd>regrettably</trd>, <trd>regretfully</trd>
       </trdgrp>
       <trd lng="de">mit Bedauern</trd>
       <trdgrp lng="fr">
         <trd>&agrave; <ind>regret</ind></trd>,
         <trd>avec <ind>regret</ind></trd>
       </trdgrp>
       <trd lng="he">&bet;<ind>&tsadi;&ayin;&resh;</ind></trd>
       <trd lng="hu">sajn&aacute;lattal</trd>
       <trd lng="nl">met <ind>spijt</ind></trd>
       <trd lng="ru">&c_s;
&c_s;&c_o;&c_zh;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je;&c_m;</trd>
</drv>

<drv mrk="bedaux.0inda">
       <kap><tld/>inda</kap>
       <snc>
         <dif>
           Kiu meritas, naskas <tld/>on:
           <ekz>
             <tld/>inda okaza&jcirc;o.
           </ekz>
         </dif>
       </snc>
       <trd lng="be">&c_v;&c_a;&c_r;&c_t;&c_y; <ind>&c_zh;&c_a;&c_l;&c_ju;</ind></trd>
       <trdgrp lng="de">
          <trd>bedauerlich</trd>,
          <trd>bedauernswert</trd>
       </trdgrp>
       <trdgrp lng="en">
         <trd>regrettable</trd>
         <trd>unfortunate</trd>
       </trdgrp>
       <trdgrp lng="fr">
         <trd>regrettable</trd>,
         <trd>f&acirc;cheux</trd>
       </trdgrp>
       <trd lng="he">&mem;<ind>&tsadi;&ayin;&resh;</ind></trd>
       <trd lng="hu">sajn&aacute;latos</trd>
       <trd lng="nl">spijtig</trd>
       <trd lng="ru">&c_z;&c_a;&c_s;&c_l;&c_u;&c_zh;&c_i;&c_v;&c_a;&c_ju;&c_shch;&c_i;&c_j; &c_s;&c_o;&c_zh;&c_a;&c_l;&c_je;&c_n;&c_i;&c_ja;</trd>
</drv>

<drv mrk="bedaux.0inde">
       <kap><tld/>inde</kap>
       <snc>
         <dif>
           La&ubreve; <tld/>inda maniero, doma&gcirc;e:
           <ekz>
             <tld/>inde mi ne povos veni.
           </ekz>
         </dif>
       </snc>
       <trd lng="be">&c_n;&c_a;&c_zh;&c_a;&c_l;&c_mol;</trd>
       <trdgrp lng="de">
          <trd>bedauerlicherweise</trd>,
          <trd>leider</trd>
       </trdgrp>
       <trd lng="en">unfortunately</trd>
       <trd lng="hu">lamentablemente</trd>
       <trd lng="fr">malheureusement<klr tip="ind"> (de fa&ccedil;on
       regrettable)</klr></trd>
       <trdgrp lng="he">
         <trd>&lamed;&mem;&resh;&bet;&he;
&he;<ind>&tsadi;&ayin;&resh;</ind></trd>,
         <trd>&lamed;&tsadi;&ayin;&resh;&yod;</trd>
       </trdgrp>
       <trd lng="hu">sajnos</trd>
       <trdgrp lng="nl">
         <trd>helaas</trd>,
         <trd>spijtig genoeg</trd>
       </trdgrp>
       <trd lng="ru">&c_k;
&c_s;&c_o;&c_zh;&c_a;&c_l;&c_je;&c_n;&c_i;&c_ju;</trd>
</drv>
</art>
