<art mrk="$Id$">
<kap>
<rad>gerbil</rad>/o
</kap>
<drv mrk="gerbil.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="hamstr1.0oj">hamstredoj</ref>
      (<trd lng="la">Gerbillus</trd>), iom simila al rato, vivanta en Azio
      kaj norda Afriko.
    </dif>
  </snc>
  <trd lng="ca">jerbu</trd>
  <trd lng="de">Rennmaus</trd>
  <trd lng="fr">gerbille</trd>
  <trd lng="en">gerbil</trd>
  <trd lng="es">jerbo</trd>
  <trd lng="hu">versenyeg&eacute;r</trd>
  <trdgrp lng="nl">
    <trd>gerbil</trd>,
    <trd>woestijnrat</trd>
  </trdgrp>
  <trd lng="pt">gerbila</trd>
</drv>
</art>

