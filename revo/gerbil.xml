<vortaro>
<art mrk="$Id$">
<kap>
<rad>gerbil</rad>/o
</kap>
<drv mrk="gerbil.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="genr.0o">Genro</ref> el familio 
      <ref tip="malprt" cel="hamstr1.0oj">hamstredoj</ref>
      (<trd lng="la">Gerbillus</trd>), iom simila al rato, vivanta en
      Azio
      kaj norda Afriko.
    </dif>
  </snc>
  <trd lng="ca">jerbu</trd>
  <trd lng="de">Rennmaus</trd>
  <trd lng="fr">gerbille</trd>
  <trd lng="en">gerbil</trd>
  <trd lng="es">jerbo</trd>
  <trd lng="hu">versenyeg&eacute;r</trd>
  <trdgrp lng="nl">
    <trd>gerbil</trd>,
    <trd>woestijnrat</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>gerbo</trd>,
    <trd>gerbila</trd>
  </trdgrp>
</drv>
</art>

