<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: galinu.xml,v 1.23 2023/10/15 06:07:00 revo Exp $">
<kap>
  <rad>galinul</rad>/o <fnt><bib>PV</bib></fnt>,
  <var><kap><rad var="o">galinol</rad>/o <fnt><bib>Viki</bib></fnt></kap></var>
</kap>
<drv mrk="galinu.0o">
  <kap><tld/>o,
    <var><kap><tld var="o"/>o</kap></var>
  </kap>
  <snc mrk="galinu.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el la familio <ref tip="malprt" cel="raled.0oj">raledoj</ref>
      (<trd lng="la">Gallinula</trd>):
      <ekz>
        observeblaj birdoj estas <klr>[&#x2026;]</klr> la fuliko, la platbeka anaso, la
        verdkrura <tld var="o"/>o, la malgranda grebo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=39280">Gvadalaharo (Hispanio)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld var="o"/>oj povas piediri lerte per siaj fortaj kruroj kaj havas longajn fingrojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=129592">Galinolo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/7/7e/Common_moorhen_(Gallinula_chloropus)_with_worm.jpg/640px-Common_moorhen_(Gallinula_chloropus)_with_worm.jpg" prm="CC BY-SA 4.0"><tld/>o (Gallinula chloropus), &ccirc;e Oxfordshire
        <fnt>
          <aut>Charles J Sharp, propra verko</aut>
          <lok><url ref="&WCW;/File:Common_moorhen_(Gallinula_chloropus)_with_worm.jpg">&WCW;/File:Common_moorhen_(Gallinula_chloropus)_with_worm.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <ref tip="super" cel="kok.akvo0o">akvokoko</ref>
  <ref tip="vid" cel="fulik.0o">fuliko</ref>
  <trd lng="be">&c_z;&c_ja;&c_l;&c_jo;&c_n;&c_a;&c_n;&c_o;&c_zh;&c_k;&c_a;</trd>
  <trd lng="ca"><ind>polla</ind> d'aigua</trd>
  <trd lng="cs">sl&iacute;pka zelenonoh&aacute;</trd>
  <trdgrp lng="de">
    <trd>Teichralle</trd>,
    <trd>Teichhuhn</trd>
  </trdgrp>
  <trd lng="en">moorhen</trd>
  <trd lng="fr">poule d'eau</trd>
  <trd lng="hu">v&iacute;zity&uacute;k</trd>
  <trd lng="ja">&#x30D0;&#x30F3;  <pr>&#x3070;&#x3093;</pr></trd>
  <trd lng="nl">waterhoen</trd>
  <trd lng="pl">kurka wodna</trd>
  <trdgrp lng="pt">
    <trd>frango-d'&aacute;gua</trd>,
    <trd>galinhola</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_y;&c_sh;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">vodn&aacute; sliepo&ccaron;ka</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&#x43C;&#x438;&#x448;&#x43D;&#x438;&#x446;&#x44F;</trd>,
    <trd>&#x432;&#x43E;&#x434;&#x44F;&#x43D;&#x430;&#xA0;&#x43A;&#x443;&#x440;&#x43E;&#x447;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>
</art>
