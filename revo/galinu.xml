<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: galinu.xml,v 1.24 2024/11/02 01:54:17 revo Exp $">
<kap>
  <rad>galinol</rad>/o <fnt><bib>PIV1</bib></fnt>,
  <var><kap><rad var="u">galinul</rad>/o <fnt><bib>PV</bib></fnt></kap></var>
</kap>
<drv mrk="galinu.0o">
  <kap><tld/>o,
    <var><kap><tld var="u"/>o</kap></var>
  </kap>
  <snc mrk="galinu.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el la familio <ref
      tip="malprt" cel="raled.0oj">raledoj</ref>
      (<trd lng="la">Gallinula</trd>):
      <ekz>
        nur la plendema krio de la <tld/>o kaj la ra&ubreve;ka grako de la kreko interrompas la solenan
        kvieton &ccirc;irka&ubreve; la akvoku&scirc;ejo
        <fnt>
          <aut>Jerome K. Jerome</aut>
          <vrk><url
          ref="https://esperantaretradio.blogspot.com/2018/10/tri-viroj-en-boato-2-parto-de-la-dua.html">
            Tri viroj en boato (2) - parto de la dua &ccirc;apitro</url></vrk>
          <lok>Esperanta Retradio, 2018-10-12</lok>
        </fnt>;
      </ekz>
      <ekz>
        multaj birdoj videblis: pelikanoj, kormoranoj, anseroj, cignoj, vaneloj, <tld/>oj, falkoj
        kaj multaj aliaj specoj
        <fnt>
          <aut>Jean Ripoche</aut>
          <vrk>Rumanio estas vizitinda lando</vrk>
          <lok>Internacia Fervojisto 2012.5, pa&gcirc;o 95</lok>
        </fnt>;
      </ekz>
      <ekz>
        observeblaj birdoj estas <klr>[&#8230;]</klr> la fuliko, la platbeka anaso, la
        verdkrura <tld/>o, la malgranda grebo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=39280">Gvadalaharo (Hispanio)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
    lok="&WCU;/thumb/7/7e/Common_moorhen_(Gallinula_chloropus)_with_worm.jpg/640px-Common_moorhen_(Gallinula_chloropus)_with_worm.jpg"
    prm="CC BY-SA 4.0"><tld/>o (Gallinula chloropus), &ccirc;e Oxfordshire
        <fnt>
          <aut>Charles J Sharp, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Common_moorhen_(Gallinula_chloropus)_with_worm.jpg">&WCW;/File:Common_moorhen_(Gallinula_chloropus)_with_worm.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <ref tip="super" cel="kok.akvo0o">akvokoko</ref>
  <ref tip="vid" cel="fulik.0o">fuliko</ref>
  <trd lng="be">&c_z;&c_ja;&c_l;&c_jo;&c_n;&c_a;&c_n;&c_o;&c_zh;&c_k;&c_a;</trd>
  <trd lng="ca"><ind>polla</ind> d'aigua</trd>
  <trd lng="cs">sl&iacute;pka zelenonoh&aacute;</trd>
  <trdgrp lng="de">
    <trd>Teichralle</trd>,
    <trd>Teichhuhn</trd>
  </trdgrp>
  <trd lng="en">moorhen</trd>
  <trd lng="fr">poule d'eau</trd>
  <trd lng="hu">v&iacute;zity&uacute;k</trd>
  <trdgrp lng="ja">
    <trd>&#x30D0;&#x30F3;  <pr>&#12400;&#12435;</pr></trd>,
    <trd>&#40429; <pr>&#12400;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">waterhoen</trd>
  <trd lng="pl">kurka wodna</trd>
  <trdgrp lng="pt">
    <trd>frango-d'&aacute;gua</trd>,
    <trd>galinhola</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_y;&c_sh;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">vodn&aacute; sliepo&ccaron;ka</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&#x43C;&c_i;&c_sh;&#x43D;&c_i;&c_c;&#x44F;</trd>,
    <trd>&c_v;&#x43E;&c_d;&#x44F;&#x43D;&c_a;&#xA0;&#x43A;&c_u;&c_r;&#x43E;&c_ch;&#x43A;&c_a;</trd>
  </trdgrp>
</drv>
</art>
