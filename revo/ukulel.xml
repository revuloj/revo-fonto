<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ukulel.xml,v 1.8 2022/03/28 11:08:53 revo Exp $">
<kap>
   <rad>ukulel</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ukulel.0o">
   <kap><tld/>o</kap>
   <snc mrk="ukulel.0o.MUZ">
     <uzo tip="fak">MUZ</uzo>
     <dif>
       Malgranda <ref tip="lst" cel="instru2.kord0o.MUZ" lst="voko:kordinstrumentoj">kordinstrumento</ref> gitarsimila kun
       kvar
       kordoj, portugaldevena kaj populari&gcirc;inta tra la havaja muziko:
       <ekz>
         Paul McCartney ludis bele <klr>...</klr> per la <tld/>o
         <fnt>
         <aut>Mateo</aut>,
         <vrk><url ref="http://mateo-au.blogspot.com">La Sekreta Vivo de
         Mateo</url></vrk>,
         <lok>La &Gcirc;ojo de la Ukulelo!, 2005-12-31</lok>
         </fnt>.
       </ekz>
     </dif>
   </snc>
  <trdgrp lng="ca">
     <trd>ukulele</trd>,
     <trd>ukelele</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>havajsk&#xE1; kytara</trd>,
    <trd>ukulele</trd>
  </trdgrp>
  <trd lng="de">Ukulele</trd>
  <trd lng="en">ukulele</trd>
  <trdgrp lng="fi">
     <trd>havaijinkitara</trd>,
     <trd>ukulele</trd>
   </trdgrp>
  <trd lng="fr">ukul&eacute;l&eacute;</trd>
  <trd lng="id">ukulele</trd>
  <trd lng="it">ukulele</trd>
  <trdgrp lng="sk">
    <trd>havajsk&#xE1; gitara</trd>,
    <trd>ukulele</trd>
  </trdgrp>
  <trd lng="uk">&#x433;&#x430;&#x432;&#x430;&#x439;&#x441;&#x44C;&#x43A;&#x430; &#x433;&#x456;&#x442;&#x430;&#x440;&#x430;</trd>
</drv>
</art>

