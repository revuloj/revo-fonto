<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: harpun.xml,v 1.26 2023/11/15 16:45:17 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>harpun</rad>/o
</kap>
<drv mrk="harpun.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Sagforma <ref tip="super" cel="lanc.0o">lanco</ref>
      provizita per longa &scirc;nuro kaj uzata por
      kapti grandajn fi&scirc;ojn, balenojn, ktp:
      <ekz>
        la <tld/>on oni enpu&scirc;is al la rosmaro en la bruston tiel, ke radio da varma
        sango el&scirc;prucis trans la glacion kiel fontano
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        granda <tld/>o estras muntita sur la &scirc;ipo kaj estas pafita al la cetaco
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=243687">Harpuno</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_p;&c_u;&c_n;</trd>
  <trd lng="br">treant</trd>
  <trd lng="ca">arp&oacute;</trd>
  <trd lng="cs">harpuna</trd>
  <trd lng="de">Harpune</trd>
  <trd lng="el">&kappa;&alfa;&my;&alfa_ton;&kappa;&jota;</trd>
  <trd lng="en">harpoon</trd>
  <trd lng="es">arp&oacute;n</trd>
  <trd lng="fr">harpon</trd>
  <trd lng="hu">szigony</trd>
  <trd lng="it">arpione</trd>
  <trd lng="ja">&#x929B;  <pr>&#x3082;&#x308A;</pr></trd>
  <trd lng="nl">harpoen</trd>
  <trd lng="pl">harpun</trd>
  <trd lng="pt">arp&atilde;o</trd>
  <trd lng="ru">&c_g;&c_a;&c_r;&c_p;&c_u;&c_n;</trd>
  <trd lng="sk">harp&uacute;na</trd>
  <trd lng="sv">harpun</trd>
  <trd lng="uk">&#x433;&#x430;&#x440;&#x43F;&#x443;&#x43D;</trd>
</drv>

<drv mrk="harpun.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="pik.tra0i">Trapiki</ref>
      per <tld/>o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Sumatra ardeo <klr>[&#x2026;]</klr> man&gcirc;as en neprofunda akvo, kie &gcirc;i <tld/>as fi&scirc;ojn
        per sia longa, akrapinta beko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=416304">Sumatra ardeo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_p;&c_u;&c_n;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="br">treanti&ntilde;<klr tip="ind"> (gant un treant)</klr>
  </trd>
  <trd lng="ca">arponar</trd>
  <trd lng="cs">harpunovat</trd>
  <trd lng="de">harpunieren</trd>
  <trd lng="en">harpoon</trd>
  <trd lng="es">arponear</trd>
  <trd lng="fr">harponner</trd>
  <trdgrp lng="hu">
    <trd>szigonyoz</trd>,
    <trd>megszigonyoz</trd>
  </trdgrp>
  <trd lng="it">arpionare</trd>
  <trd lng="ja">&#x3082;&#x308A;&#x3092;&#x6483;&#x3061;&#x8FBC;&#x3080;  <pr>&#x3082;&#x308A;&#x3092;&#x3046;&#x3061;&#x3053;&#x3080;</pr></trd>
  <trd lng="nl">harpoeneren</trd>
  <trdgrp lng="pl">
    <trd>przebi&cacute; <ind>harpunem</ind></trd>,
    <trd>trafi&cacute; <ind>harpunem</ind></trd>
  </trdgrp>
  <trd lng="pt">arpoar</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_r;&c_p;&c_u;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_a;&c_r;&c_p;&c_u;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">harp&uacute;nova&tcaron;</trd>
  <trd lng="sv">harpunera</trd>
  <trd lng="uk">&#x433;&#x430;&#x440;&#x43F;&#x443;&#x43D;&#x438;&#x442;&#x438;</trd>
  <trd lng="zh">&#x9B5A;&#x53C9; <pr>y&#xFA;ch&#x101;</pr></trd>
</drv>
</art>
