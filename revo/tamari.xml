<kap>
  <rad>tamarind</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="tamari.0o">
  <kap><tld/>o</kap>
  <snc mrk="tamari.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Tamarindus</trd>), kreskanta en varmegaj landoj:
      <ekz>
        la <tld/>o <klr>...</klr> havas aspekton de dronanto, kiu vane
        etendas siajn manojn al la &ccirc;ielo
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 17a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_m;&c_a;&c_r;&c_y;&c_n;&c_d; &c_ib;&c_n;&c_d;&c_y;&c_j;&c_s;&c_k;&c_ib;</trd>,
    <trd>&c_ib;&c_n;&c_d;&c_y;&c_j;&c_s;&c_k;&c_ib; &c_f;&c_ib;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">tamarinde</trd>
  <trd lng="cs">tamarind</trd>
  <trd lng="de">Tamarinde</trd>
  <trd lng="en">tamarind</trd>
  <trd lng="fr">tamarinier</trd>
  <trd lng="hu">tamarindus(fa)</trd>
  <trdgrp lng="ja">
    <trd>&#x30BF;&#x30DE;&#x30EA;&#x30F3;&#x30C9;</trd>,
    <trd>&#x30BF;&#x30DE;&#x30EA;&#x30F3;&#x30C9;&#x306E;&#x5B9F;  <pr>&#x30BF;&#x30DE;&#x30EA;&#x30F3;&#x30C9;&#x306E;&#x307F;</pr></trd>
  </trdgrp>
  <trd lng="nl">tamarinde</trd>
  <trdgrp lng="pl">
    <trd>tamaryndowiec</trd>,
    <trd>daktyl indyjski</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>tamarindo</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_a;&c_m;&c_a;&c_r;&c_i;&c_n;&c_d;</trd>,
    <trd>&c_i;&c_n;&c_d;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j;
    &c_f;&c_i;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">tamarind</trd>
  <trd lng="uk">&c_t;&c_a;&#x43C;&c_a;&c_r;&c_i;&#x43D;&c_d;</trd>
  <trdgrp lng="zh">
    <trd>&#x9178;&#x8C46; <pr>su&#x101;nd&#xF2;u</pr></trd>,
    <trd>&#x7F57;&#x671B;&#x5B50; <pr>lu&#xF3;w&#xE0;ngz&#x1D0;</pr></trd>,
    <trd>&#x7F85;&#x671B;&#x5B50; <pr>lu&#xF3;w&#xE0;ngz&#x1D0;</pr></trd>
  </trdgrp>
</drv>
</art>
