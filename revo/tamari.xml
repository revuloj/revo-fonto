<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tamarind</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="tamari.0o">
  <kap><tld/>o</kap>
  <snc mrk="tamari.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Tamarindus</trd>), kreskanta en varmegaj landoj:
      <ekz>
        la <tld/>o <klr>...</klr> havas aspekton de dronanto, kiu vane
        etendas siajn manojn al la &ccirc;ielo
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 17a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Tamarinde</trd>
  <trd lng="en">tamarind</trd>
  <trd lng="fr">tamarinier</trd>
  <trd lng="hu">tamarindus(fa)</trd>
  <trd lng="nl">tamarinde</trd>
  <trdgrp lng="pl">
    <trd>tamaryndowiec</trd>,
    <trd>daktyl indyjski</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>tamarindo</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_a;&c_m;&c_a;&c_r;&c_i;&c_n;&c_d;</trd>,
    <trd>&c_i;&c_n;&c_d;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j;
    &c_f;&c_i;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
