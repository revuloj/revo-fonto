<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>lazur</rad>/o
</kap>
<drv mrk="lazur.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Helblua <ref tip="super" cel="kolor.0o.propre">koloro</ref> de
      &ccirc;ielo, maro kaj similaj:
      <ekz>
        <tld/>a okulo;
      </ekz>
      <ekz>
        <ind><tld/>&scirc;tono</ind>.
        <trd lng="hu">laz&uacute;rk&odblac;</trd>
        <trd lng="ru">&c_l;&c_a;&c_z;&c_u;&c_r;&c_i;&c_t;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_l;&c_a;&c_k;&c_ib;&c_t;</trd>
  <trdgrp lng="de">
    <trd>Azur</trd>,
    <trd>Himmelsblau</trd>
  </trdgrp>
  <trd lng="el">&gamma;&alfa;&lambda;&alfa;&ny;&omikron_ton;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>azure</trd>,
    <trd>sky <ind>blue</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>azur<klr> (couleur)</klr></trd>,
    <trd>bleu ciel<klr> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>az&uacute;r <klr tip="ind">(sz&iacute;n)</klr></trd>,
    <trd>&eacute;gsz&iacute;nk&eacute;k <klr tip="ind">(sz&iacute;n)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>azuur</trd>,
    <trd>hemelsblauw</trd>
  </trdgrp>
  <trd lng="pl">lazur</trd>
  <trd lng="pt">azul-celeste</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_z;&c_u;&c_r;&c_mol;</trd>,
    <trd>&c_n;&c_je;&c_b;&c_je;&c_s;&c_n;&c_a;&c_ja; &c_s;&c_i;&c_n;&c_je;&c_v;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="lazur.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Helblua kiel &ccirc;ielo, maro kaj similaj.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_l;&c_a;&c_k;&c_ib;&c_t;&c_n;&c_y;</trd>
  <trdgrp lng="en">
    <trd>azure</trd>,
    <trd>sky <ind>blue</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>azur<klr> (adj.)</klr></trd>,
    <trd>azur&eacute;</trd>,
    <trd>bleu ciel<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>az&uacute;r</trd>,
    <trd>&eacute;gsz&iacute;nk&eacute;k</trd>
  </trdgrp>
  <trd lng="pl">lazurowy</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_z;&c_u;&c_r;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_b;&c_je;&c_s;&c_n;&c_o;-&c_g;&c_o;&c_l;&c_u;&c_b;&c_o;&c_j;</trd>
  </trdgrp>
</drv>
</art>
