<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lazur.xml,v 1.25 2024/09/30 18:37:56 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>lazur</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="lazur.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Helblua <ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">koloro</ref> de
      &ccirc;ielo, maro kaj similaj:
      <ekz>
        la vintra &ccirc;ielo brilis per la plej pura <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi vidis la helan <tld/>on super mi, la
        sunon kaj la verdan akvon de la oceano
        <fnt>
          <bib>FdO</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="sxton.lazur0o">lazur&scirc;tono</ref>,
    <ref cel="bord.Lazura_0o"><tld lit="L"/>a Bordo</ref>
  </refgrp>
  <trd lng="be">&c_b;&c_l;&c_a;&c_k;&c_ib;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>azurov&aacute; mod&rcaron;</trd>,
    <trd>lazurov&aacute; mod&rcaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Azur</trd>,
    <trd>Himmelsblau</trd>
  </trdgrp>
  <trd lng="el">&gamma;&alfa;&lambda;&alfa;&ny;&omikron_ton;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>azure</trd>,
    <trd>sky <ind>blue</ind></trd>
  </trdgrp>
  <trd lng="es">azur</trd>
  <trdgrp lng="fr">
    <trd>azur<klr> (couleur)</klr></trd>,
    <trd>bleu ciel<klr> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>az&uacute;r <klr tip="ind">(sz&iacute;n)</klr></trd>,
    <trd>&eacute;gsz&iacute;nk&eacute;k <klr tip="ind">(sz&iacute;n)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#38738;  <pr>&#12354;&#x304A;</pr></trd>,
    <trd>&#x7D3A;&#38738;  <pr>&#12371;&#12435;&#12376;&#12423;&#12358;</pr></trd>,
    <trd>&#x7D3A;&#x78A7;  <pr>&#12371;&#12435;&#x307A;&#x304D;</pr></trd>,
    <trd>&#x308B;&#x308A;&#33394;  <pr>&#x308B;&#x308A;&#12356;&#x308D;</pr></trd>,
    <trd>&#38738;&#x7A7A;  <pr>&#12354;&#x304A;&#x305E;&#12425;</pr></trd>,
    <trd>&#x78A7;&#x7A7A;  <pr>&#12408;&#x304D;&#x304F;&#12358;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>azuur</trd>,
    <trd>hemelsblauw</trd>
  </trdgrp>
  <trd lng="pl">lazur</trd>
  <trd lng="pt">azul-celeste</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_z;&c_u;&c_r;&c_mol;</trd>,
    <trd>&c_n;&c_je;&c_b;&c_je;&c_s;&c_n;&c_a;&c_ja;
    &c_s;&c_i;&c_n;&c_je;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">laz&uacute;rov&aacute; modr&aacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_l;&c_a;&c_z;&c_u;&c_r;</trd>,
    <trd>&c_b;&c_l;&c_a;&c_k;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_g;&c_o;&c_l;&c_u;&c_b;&c_ib;&c_n;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#22825;&#x84DD;&#33394; <pr>ti&amacron;nl&#xE1;ns&#xE8;</pr></trd>,
    <trd>&#22825;&#x85CD;&#33394; <pr>ti&amacron;nl&#xE1;ns&#xE8;</pr></trd>,
    <trd>&#x77F3;&#38738; <pr>sh&#xED;q&#x12B;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lazur.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Hele <ref tip="super" cel="blu.0a">blua</ref> kiel &ccirc;ielo,
      maro kaj similaj:
      <ekz>
        kelkdeko da kanaloj <klr>[&#8230;]</klr> &scirc;ajnis
        kvaza&ubreve; reto el ar&gcirc;entaj kaj <tld/>aj
        &scirc;nuroj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        montoj blankaj pro ne&gcirc;amasoj, <tld/>aj lagoj, profundaj valoj
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_l;&c_a;&c_k;&c_ib;&c_t;&c_n;&c_y;</trd>
  <trdgrp lng="en">
    <trd>azure</trd>,
    <trd>sky <ind>blue</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>azur<klr> (adj.)</klr></trd>,
    <trd>azur&eacute;</trd>,
    <trd>bleu ciel<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>az&uacute;r</trd>,
    <trd>&eacute;gsz&iacute;nk&eacute;k</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#38738;&#12356;  <pr>&#12354;&#x304A;&#12356;</pr></trd>,
    <trd>&#x7D3A;&#38738;&#x306E;  <pr>&#12371;&#12435;&#12354;&#x304A;&#x306E;</pr></trd>,
    <trd>&#x7D3A;&#x78A7;&#x306E;  <pr>&#12371;&#12435;&#x307A;&#x304D;&#x306E;</pr></trd>,
    <trd>&#x308B;&#x308A;&#33394;&#x306E;  <pr>&#x308B;&#x308A;&#12356;&#x308D;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="pl">lazurowy</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_z;&c_u;&c_r;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_b;&c_je;&c_s;&c_n;&c_o;-&c_g;&c_o;&c_l;&c_u;&c_b;&c_o;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_l;&c_a;&c_k;&c_i;&c_t;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_l;&c_a;&c_z;&c_u;&c_r;&c_o;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_l;&c_a;&c_z;&c_u;&c_r;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_b;&c_je;&c_s;&c_n;&c_o;-&c_g;&c_o;&c_l;&c_u;&c_b;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x851A;&#x84DD; <pr>w&#xE8;il&#xE1;n</pr></trd>,
    <trd>&#x851A;&#x85CD; <pr>w&#xE8;il&#xE1;n</pr></trd>,
    <trd>&#22825;&#x84DD; <pr>ti&amacron;nl&#xE1;n</pr></trd>,
    <trd>&#22825;&#x85CD; <pr>ti&amacron;nl&#xE1;n</pr></trd>,
    <trd>&#x6E5B;&#x84DD; <pr>zh&#xE0;nl&#xE1;n</pr></trd>,
    <trd>&#x6E5B;&#x85CD; <pr>zh&#xE0;nl&#xE1;n</pr></trd>,
    <trd>&#x77F3;&#38738; <pr>sh&#xED;q&#x12B;ng</pr></trd>
  </trdgrp>
</drv>
</art>
