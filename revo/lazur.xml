<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>lazur</rad>/o
</kap>
<drv mrk="lazur.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Blua koloro de la &ccirc;ielo:
      <ekz>
        <ind><tld/>a</ind> maro, okulo;
        <trdgrp lng="ru">
          <trd>&c_l;&c_a;&c_z;&c_u;&c_r;&c_n;&c_y;&c_j;</trd>,
          <trd>&c_n;&c_je;&c_b;&c_je;&c_s;&c_n;&c_o;-&c_g;&c_o;&c_l;&c_u;&c_b;&c_o;&c_j;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind><tld/>&scirc;tono</ind>.
        <trd lng="ru">&c_l;&c_a;&c_z;&c_u;&c_r;&c_i;&c_t;</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>azure</trd>,
    <trd>sky <ind>blue</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>azuur</trd>,
    <trd>hemelsblauw</trd>
  </trdgrp>
  <trd lng="pl">niebieski</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_z;&c_u;&c_r;&c_mol;</trd>,
    <trd>&c_n;&c_je;&c_b;&c_je;&c_s;&c_n;&c_a;&c_ja; &c_s;&c_i;&c_n;&c_je;&c_v;&c_a;</trd>
  </trdgrp>
</drv>
</art>
