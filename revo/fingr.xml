  <trd lng="it">anulare<klr> (dito)</klr></trd>
  <trd lng="nl">ringvinger</trd>
  <trd lng="pl"><ind>palec</ind> serdeczny</trd>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_y;&c_m;&c_ja;&c_n;&c_n;&c_y;&c_j; &c_p;&c_a;&c_l;&c_je;&c_c;</trd>
  <trd lng="sk">prstenn&iacute;k</trd>
  <trd lng="sv">ringfinger</trd>
  <trd lng="uk">&c_b;&c_je;&c_z;&c_ib;&#x43C;&c_je;&#x43D;&#x43D;&c_i;&c_j; &#x43F;&c_a;&#x43B;&c_je;&c_c;&#x44C;</trd>
</drv>

<drv mrk="fingr.inter0igilamanojn">
  <kap>inter<tld/>igi la manojn</kap>
  <snc mrk="fingr.inter0igilamanojn.gesti">
    <dif>
      Intermeti la <tld/>ojn de unu mano alterne inter la <tld/>ojn de
      la alia:
      <ekz>
        la juna homo faris sopiran &gcirc;emon, inter<tld/>igis la
        manojn kaj klinis la kapon sur la bruston
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_sh;&c_ch;&c_e;&c_p;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol; <ind>&c_p;&c_a;&c_l;&c_mol;&c_c;&c_y;</ind></trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_p;&c_l;&c_ja;&c_t;&c_a;&c_c;&c_mol; <ind>&c_p;&c_a;&c_l;&c_mol;&c_c;&c_y;</ind></trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_p;&c_l;&c_i;&c_t;&c_a;&c_m; &c_p;&c_r;&c_malmol;&c_s;&c_t;&c_i;</trd>
  <trd lng="ca">encreuar les mans</trd>
  <trd lng="de">H&auml;nde falten</trd>
  <trd lng="fr">croiser les mains</trd>
  <trd lng="nl">de <ind>handen</ind> vouwen</trd>
