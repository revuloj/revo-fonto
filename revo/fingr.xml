</drv>

<drv mrk="fingr.longa0o">
  <kap>longa <tld/>o,
  <var><kap>mez<tld/>o <fnt><bib>PIV2</bib></fnt></kap></var>,
  <var><kap>meza <tld/>o</kap></var></kap>
  <snc mrk="fingr.longa0o.KOMUNE">
    <uzo tip="klr">(nur pri longa fingro)</uzo>
    <dif>
      <tld lit="F"/>o neordinare longa:
      <ekz>
        &scirc;i spiris sur min kaj gratis mian viza&gcirc;on per siaj longaj
        <tld/>oj
        <fnt>
          <bib>ElektFab</bib>
          <lok>La bremenaj muzikistoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="fingr.longa0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Plej longa, tria <ref tip="super" cel="fingr.0o.ANA"><tld/>o</ref>:
      <ekz>
        la mez<tld/>o a&ubreve; longa <tld/>o estas <klr>[...]</klr> la meza el
        la kvin fingroj kaj trovi&gcirc;as inter la montro<tld/>o kaj la
        ringo<tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=57940">Mezfingro</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la nuna &ccirc;evalo mar&scirc;as per meza <tld/>o de siaj piedoj pro
        adapti&gcirc;o al siaj aktualaj vivkondi&ccirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>J.-Y. Santerre</aut>
          <vrk><url ref="http://www.esperanto.be/fel/mon/006864.html">Dombestoj:
          &Ccirc;evalrasoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_ja;&c_r;&c_e;&c_d;&c_n;&c_ib; <ind>&c_p;&c_a;&c_l;&c_je;&c_c;</ind></trd>,
    <trd>&c_s;&c_ja;&c_r;&c_e;&c_d;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_r;&c_je;&c_d;&c_je;&c_n; &c_p;&c_r;&c_malmol;&c_s;&c_t;</trd>
  <trdgrp lng="ca">
    <trd>dit del mig</trd>,
    <trd>dit llarg</trd>
  </trdgrp>
  <trd lng="cs">prost&rcaron;edn&iacute;&ccaron;ek</trd>
  <trd lng="de">Mittelfinger</trd>
  <trd lng="es"><ind>dedo</ind> medio</trd>
  <trd lng="fa">&a_A;&a_n;&f_g;&a_sh;&a_t; &a_w;&a_s;&a_T;</trd>
  <trdgrp lng="fr">
    <trd>majeur<klr tip="ind"> (doigt)</klr></trd>,
    <trd>m&eacute;dius</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&ouml;z&eacute;pujj</trd>,
    <trd>k&ouml;z&eacute;ps&odblac; ujj</trd>
  </trdgrp>
  <trd lng="id"><ind>jari</ind> tengah</trd>
  <trd lng="it">medio<klr> (dito)</klr></trd>
  <trd lng="nl">middenvinger</trd>
  <trd lng="pl"><ind>palec</ind> &sacute;rodkowy</trd>
  <trd lng="ru">&c_s;&c_r;&c_je;&c_d;&c_n;&c_i;&c_j; &c_p;&c_a;&c_l;&c_je;&c_c;</trd>
  <trd lng="sk">prostredn&iacute;k</trd>
  <trd lng="sv">l&aring;ngfinger</trd>
  <trd lng="uk">&c_s;&c_je;&c_r;&c_je;&c_d;&#x43D;&c_ib;&c_j; &#x43F;&c_a;&#x43B;&c_je;&c_c;&#x44C;</trd>
</drv>

<drv mrk="fingr.malgranda0o">
  <kap>malgranda <tld/>o,
  <var><kap>et<tld/>o</kap></var></kap>
  <snc mrk="fingr.malgranda0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Plej eta <ref tip="super" cel="fingr.0o.ANA"><tld/>o</ref>, kvina
      nombrante de la dika <tld/>o:
      <ekz>
        tiele diru al ili: mia malgranda <tld/>o estas pli dika, ol la
        lumboj de mia patro
        <fnt><bib>MT</bib> <lok>&Re1; 12:10</lok></fnt>;
      </ekz>
      <ekz>
        la plej malgranda nano ne pli alta ol mia malgranda <tld/>o
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fi&scirc;kaptisto kaj lia edzino</lok>
        </fnt>;
      </ekz>
      <ekz>
        Jaguarpiedul, indiano, sidadis anta&ubreve; sia vigvamo kaj
        observante siajn pied<tld/>ojn pensadis pri tio: kial li ne
        povas movi la et<tld/>on tiel, kiel la ceterajn fingrojn?
        <fnt><aut>S. Szathm&aacute;ri</aut> <vrk>La guarbo</vrk></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_l;&c_y; <ind>&c_p;&c_a;&c_l;&c_je;&c_c;</ind></trd>,
    <trd>&c_m;&c_je;&c_z;&c_ja;&c_n;&c_je;&c_c;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_k;&c_u;&c_t;&c_r;&c_je;</trd>,
    <trd>&c_m;&c_a;&c_l;&c_malmol;&c_k; &c_p;&c_r;&c_malmol;&c_s;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>menovell</trd>,
    <trd>dit petit</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mal&iacute;k</trd>,
    <trd>mal&iacute;&ccaron;ek</trd>
  </trdgrp>
  <trd lng="de">kleiner <ind>Finger</ind></trd>
  <trd lng="es">me&ntilde;ique</trd>
  <trd lng="fa">&a_A;&a_n;&f_g;&a_sh;&a_t; &f_k;&a_w;&f_ch;&f_k;</trd>
  <trdgrp lng="fr">
    <trd>auriculaire<klr tip="ind"> (doigt)</klr></trd>,
    <trd>petit doigt</trd>
  </trdgrp>
  <trd lng="hu">kisujj</trd>
  <trd lng="id">kelingking</trd>
  <trd lng="it">mignolo</trd>
  <trd lng="nl">pink</trd>
  <trd lng="pl">ma&lstroke;y <ind>palec</ind></trd>
  <trd lng="ru">&c_m;&c_i;&c_z;&c_i;&c_n;&c_je;&c_c;</trd>
  <trd lng="sk">mal&iacute;&ccaron;ek</trd>
  <trd lng="sv">lillfinger</trd>
  <trd lng="uk">&#x43C;&c_ib;&c_z;&c_i;&#x43D;&c_je;&c_c;&#x44C;</trd>
</drv>

<drv mrk="fingr.montra0o">
  <kap>montra <tld/>o,
  <var><kap>montro<tld/>o</kap></var></kap>
  <snc mrk="fingr.montra0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Dua <ref tip="super" cel="fingr.0o.ANA"><tld/>o</ref>,
      nombrante de la dika <tld/>o:
      <ekz>
        la montran <tld/>on li etendis al la pordo de la kabineto
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        atenteme li tiras la ar&ccirc;on, la montro<tld/>o alterne
        premas la kordon
        <fnt>
          <aut>&Scirc;. Urban</aut>
          <vrk>Mario kaj la papilioj</vrk>
          <lok>Nica Literatura Revuo, 4:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis lasinta inter la pa&gcirc;oj la dikan montro<tld/>on de sia
        maldekstra mano dum la fermado &#8208; kiel legomarkilon
        <fnt>
          <aut>M. Ende, trad. W. Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>1997</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_k;&c_a;&c_z;&c_mol;&c_n;&c_ib;&c_k; <klr tip="amb">(&c_p;&c_a;&c_l;&c_je;&c_c;)</klr></trd>,
    <trd>&c_p;&c_a;&c_k;&c_a;&c_z;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k; <klr tip="amb">(&c_p;&c_a;&c_l;&c_je;&c_c;)</klr></trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_o;&c_k;&c_a;&c_z;&c_a;&c_l;&c_je;&c_c;</trd>
  <trd lng="bo">&t_ma;&t_dza;&t_shabchu;&t_ba;&t_tsheg;&t_ma;&t_naro;&t_tsheg;</trd>
  <trd lng="ca">&iacute;ndex</trd>
  <trdgrp lng="cs">
    <trd>ukazov&aacute;k</trd>,
    <trd>ukazov&aacute;&ccaron;ek</trd>
  </trdgrp>
  <trd lng="de">Zeigefinger</trd>
  <trd lng="es"><ind>dedo</ind> &iacute;ndice</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_n;&f_g;&a_sh;&a_t; &a_A;&a_sh;&a_A;&a_r;&a_h;</trd>,
    <trd>&a_A;&a_n;&f_g;&a_sh;&a_t;
    &a_s;&a_b;&a_shadda;&a_A;&a_b;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">index<klr tip="ind"> (doigt)</klr></trd>
  <trd lng="hu">mutat&oacute;ujj</trd>
  <trd lng="id"><ind>jari</ind> telunjuk</trd>
  <trd lng="it">indice<klr> (dito)</klr></trd>
  <trd lng="nl">wijsvinger</trd>
  <trd lng="pl"><ind>palec</ind> wskazuj&aogonek;cy</trd>
  <trd lng="ru">&c_u;&c_k;&c_a;&c_z;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_p;&c_a;&c_l;&c_je;&c_c;</trd>
  <trd lng="sk">ukazov&aacute;k</trd>
  <trd lng="sv">pekfinger</trd>
  <trd lng="uk">&c_v;&#x43A;&c_a;&c_z;&c_ib;&c_v;&#x43D;&c_i;&c_j; &#x43F;&c_a;&#x43B;&c_je;&c_c;&#x44C;</trd>
</drv>

