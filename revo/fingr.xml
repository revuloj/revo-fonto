    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_je;&c_z;&c_y;&c_m;&c_je;&c_n;&c_n;&c_y; <ind>&c_p;&c_a;&c_l;&c_je;&c_c;</ind></trd>,
