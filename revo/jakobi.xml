<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Jakobi</rad>/o
</kap>
<drv mrk="jakobi.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Germanlingve: Karl <trd lng="de">Jacobi</trd>, 1804-1851.
      Germana matematikisto.
    </dif>
  </snc>
  <trd lng="fr">Jacobi</trd>
  <trd lng="en">Jacobi</trd>
  <trd lng="ru">&c_Ja;&c_k;&c_o;&c_b;&c_i;</trd>
</drv>

<drv mrk="jakobi.j0a">
  <kap>
    <tld lit="j"/>a
  </kap>
  <snc>
    <refgrp tip="vid">
      <ref cel="matric.jakobia0o"><tld lit="j"/>a matrico</ref>,
      <ref cel="determ.jakobia0anto"><tld lit="j"/>a determinanto</ref>,
      <ref cel="jakobi1.0o">jakobiano</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Jacobisch</trd>
  <trd lng="en">Jacobian</trd>
  <trd lng="fr">jacobien<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="ru">&c_ja;&c_k;&c_o;&c_b;&c_i;&c_je;&c_v;</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>