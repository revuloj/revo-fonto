<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: jakobi.xml,v 1.8 2019/03/14 07:10:14 revo Exp $">
<kap>
  <rad>Jakobi</rad>/o
</kap>
<drv mrk="jakobi.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Germanlingve: <nom>Karl</nom> <trd lng="de">Jacobi</trd>, 1804-1851.
      Germana <ref tip="lst" lst="voko:historiaj_personoj" cel="matema.0isto">matematikisto</ref>:
      <ekz>
        la <tld lit="j"/>a simbolo estas &gcirc;eneraligo de la simbolo de <nom>Legendre</nom> donita de
        <tld/>o en 1837, &gcirc;i estas de teoria intereso en modula aritmetiko kaj aliaj
        bran&ccirc;oj de nombroteorio, sed &gcirc;ia &ccirc;efa uzo estas en komputa nombroteorio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=247152"
              >Jakobia simbolo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_Ja;&c_k;&c_o;&c_b;&c_ib;</trd>
  <trd lng="ca">Jacobi</trd>
  <trd lng="fr">Jacobi</trd>
  <trd lng="en">Jacobi</trd>
  <trd lng="hu">Jacobi</trd>
  <trd lng="pl">Jacobi</trd>
  <trd lng="ru">&c_Ja;&c_k;&c_o;&c_b;&c_i;</trd>
  <trd lng="it">Jacobi</trd>
</drv>

<drv mrk="jakobi.j0a">
  <kap>
    <tld lit="j"/>a
  </kap>
  <snc>
    <refgrp tip="vid">
      <ref cel="matric.jakobia0o"><tld lit="j"/>a matrico</ref>,
      <ref cel="determ.jakobia0anto"><tld lit="j"/>a determinanto</ref>,
      <ref cel="jakobi1.0o">jakobiano</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_Ja;&c_k;&c_o;&c_b;&c_ib;</trd>
  <trdgrp lng="ca">
    <trd>jacobi&agrave;</trd>,
    <trd>jacobiana</trd>
  </trdgrp>
  <trd lng="de">Jacobisch</trd>
  <trd lng="en">Jacobian</trd>
  <trd lng="fr">jacobien<klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>Jacobi-</trd>,
    <trd>Jacobi-f&eacute;le</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>Jacobiego</trd>,
    <trd>jakobianowy</trd>
  </trdgrp>
  <trd lng="ru">&c_ja;&c_k;&c_o;&c_b;&c_i;&c_je;&c_v;</trd>
  <trd lng="it">jacobiano<klr tip="amb"> (agg.)</klr></trd>
</drv>
</art>

