<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>oniks</rad>/o
</kap>
<drv mrk="oniks.0o">
  <kap><tld/>o</kap>
  <snc mrk="oniks.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Delikata agato, prezentanta alterne helajn kaj malhelajn tavolojn,
      el kiu oni faras <ref tip="vid" cel="kame.0o.JUV">kameojn</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_o;&c_n;&c_ib;&c_k;&c_s;</trd>
  <trd lng="de">Onyx</trd>
  <trd lng="el">&omikron_ton;&ny;&ypsilon;&xi;</trd>
  <trd lng="fr">onyx</trd>
  <trd lng="hu">&oacute;nix</trd>
  <trd lng="nl">onyx</trd>
  <trd lng="pt">&ocirc;nix</trd>
  <trd lng="ru">&c_o;&c_n;&c_i;&c_k;&c_s;</trd>
</drv>
</art>
