<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>aks</rad>/o
    </kap>
    <drv mrk="aks.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="aks.0o.rotacia">
        <dif>
          Linio, &ccirc;irka&ubreve; kiu turni&gcirc;as korpo:
          <ekz>
            la <tld/>o de nia 
            globo trairas tra amba&ubreve; polusoj;
          </ekz><ekz>
            <tld/>o de rado.
          </ekz>
        </dif>
      </snc>
      <snc mrk="aks.0o.de.simetrio">
        <dif>
          Centra, al randoj paralela linio:
          <ekz>
            <tld/>o de vojo.
          </ekz>
        </dif>
      </snc>
      <snc mrk="aks.0o.FIG">
        <uzo tip="stl">FIG</uzo><dif>
          Centra, plej grava organo en iu organizo:
          <ekz>
            tiu komitato estas la 
            &ccirc;efa  <tld/>o de la societo.
          </ekz>
        </dif>
      </snc>
      <trd lng="fr">axe</trd>
      <trd lng="ru">&c_o;&c_s;&c_mol;</trd>
      <trd lng="sv">axel</trd>
    </drv>
    <drv mrk="aks.0ingo">
      <kap><tld/>ingo</kap>
      <snc>
        <dif>
          Muntopeco entenata <tld/>an aperturon por alia muntopeco.
        </dif>
        <refgrp tip="vid">
          <ref cel="pivot.0o">pivoto</ref>.
        </refgrp>
      </snc>
      <trd lng="fr">moyeu</trd>
      <trdgrp lng="ru">
        <trd>&c_v;&c_t;&c_u;&c_l;&c_k;&c_a;</trd>,
        <trd>&c_s;&c_t;&c_u;&c_p;&c_i;&c_c;&c_a;</trd>
      </trdgrp>
    </drv>
  </art>
