<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pup</rad>/o
</kap>
<drv mrk="pup.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pup.0o.ludilo">
    <dif>
      Figureto de homo, eventuale de besto, el &ccirc;ifonoj, ledo,
      ligno, plasto, uzata kiel infana ludilo, teatra rolulo a&ubreve; sor&ccirc;ilo:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sub tia diktatora re&gcirc;imo la re&gcirc;o i&gcirc;as senpova <tld/>o;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        elegantulino, nura <tld/>o<fnt>B</fnt>.
      </ekz>
    </dif>
    <trd lng="de">Puppe</trd>
    <trd lng="en">doll</trd>
    <trd lng="fr">poup&eacute;e</trd>
    <trd lng="hu">baba</trd>
    <trd lng="ru">&c_k;&c_u;&c_k;&c_l;&c_a;</trd>
    <trd lng="sv">docka</trd>
  </snc>
  <snc mrk="pup.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="krizal.0">krizalido</ref>
    <trdgrp lng="fr">
      <trd>chrysalide</trd>
    </trdgrp>
    <trd lng="hu">b&aacute;b</trd>
    <trd lng="ru">&c_k;&c_u;&c_k;&c_o;&c_l;&c_k;&c_a;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>panenka</trd>,
    <trd>loutka</trd>
  </trdgrp>
  <trd lng="es">pupa</trd>
  <trd lng="nl">pop</trd>
  <trd lng="sv">puppa</trd>
</drv>
</art>
