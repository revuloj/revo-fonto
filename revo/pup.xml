<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pup.xml,v 1.46 2024/02/17 12:12:49 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pup</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pup.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pup.0o.ludilo">
  <uzo tip="fak">SPO</uzo>
    <dif>
      Figureto de homo, eventuale de besto, el &ccirc;ifonoj, ledo,
      ligno, plasto, uzata kiel infana <ref tip="lst" cel="lud.0ilo" lst="voko:ludiloj">ludilo</ref>, teatra rolulo a&ubreve;
      sor&ccirc;ilo:
      <ekz>
        la infano ser&ccirc;is sian <tld/>on
        <fnt><bib>F</bib>, <lok>&FE; 18</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sub tia diktatora re&gcirc;imo la re&gcirc;o i&gcirc;as senpova <tld/>o;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        elegantulino, nura <tld/>o<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_l;&c_ja;&c_l;&c_mol;&c_k;&c_a;</trd>,
      <trd>&c_m;&c_a;&c_r;&c_y;&c_ja;&c_n;&c_e;&c_t;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="bg">&c_k;&c_u;&c_k;&c_l;&c_a;</trd>
    <trd lng="ca">ninot</trd>
    <trdgrp lng="de">
      <trd>Puppe</trd>,
      <trd>Spielzeugpuppe</trd>
    </trdgrp>
    <trdgrp lng="en"><trd>doll</trd>, <trd>puppet</trd></trdgrp>
    <trdgrp lng="fr">
      <trd>marionnette</trd>,
      <trd>poup&eacute;e</trd>
    </trdgrp>
    <trd lng="hu">baba</trd>
    <trd lng="id">boneka</trd>
    <trdgrp lng="pt">
      <trd>boneco</trd>,
      <trd>boneca</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_u;&c_k;&c_l;&c_a;</trd>,
      <trd>&c_m;&c_a;&c_r;&c_i;&c_o;&c_n;&c_je;&c_t;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="sv">docka</trd>
    <trdgrp lng="zh">
    <trd>&#x73A9;&#x5076; <pr>w&#xE1;n&#x1D2;u</pr></trd>,
    <trd>&#x5A03;&#x5A03; <pr>w&#xE1;wa</pr></trd>,
    <trd>&#x6D0B;&#x5A03;&#x5A03; <pr>y&#xE1;ngw&#xE1;wa</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="pup.0o.ZOO">
    <uzo tip="stl">KOMUNE</uzo>
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="krizal.0o.ZOO">krizalido</ref>
        <trdgrp lng="be">
      <trd>&c_l;&c_ja;&c_l;&c_je;&c_ch;&c_k;&c_a; <klr tip="amb">(&c_z;&c_a;&c_a;&c_l;.)</klr></trd>,
      <trd>&c_k;&c_u;&c_k;&c_a;&c_l;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="ca">pupa</trd>
    <trdgrp lng="de">
      <trd>Puppe</trd>,
      <trd>Schmetterlingspuppe</trd>
    </trdgrp>
    <trd lng="fr">chrysalide</trd>
    <trd lng="hu">b&aacute;b</trd>
    <trdgrp lng="id">
      <trd>kepompong</trd>,
      <trd>pupa</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>pupa</trd>,
      <trd>cris&aacute;lida</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_u;&c_k;&c_o;&c_l;&c_k;&c_a;</trd>
  </snc>
  <trd lng="bo">&#xF68;&#xF0B;&#xF63;&#xF66;&#xF0B;&#xF54;&#xF51;&#xF0B;&#xF40;&#xF7C;&#xF42;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>loutka</trd>,
    <trd>panenka</trd>,
    <trd>panenka <klr>(hra&ccaron;ka)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>chrysalis</trd>,
    <trd>pupa</trd>
  </trdgrp>
  <trd lng="es">mu&ntilde;eca<klr tip="amb"> (juguete)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x4EBA;&#x5F62;  <pr>&#x3072;&#x3068;&#x304C;&#x305F;</pr></trd>,
    <trd>&#x6307;&#x4EBA;&#x5F62;  <pr>&#x3086;&#x3073;&#x306B;&#x3093;&#x304E;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x3055;&#x306A;&#x304E;</trd>,
    <trd>&#x304A;&#x4EBA;&#x5F62;  <pr>&#x304A;&#x306B;&#x3093;&#x304E;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">pop</trd>
  <trdgrp lng="sk">
    <trd>b&aacute;bika</trd>,
    <trd>panenka <klr>(hra&ccaron;ka)</klr></trd>
  </trdgrp>
  <trd lng="sv">puppa</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x44F;&#x43B;&#x44C;&#x43A;&c_a;</trd>,
    <trd>&#x43C;&c_a;&c_r;&c_ib;&#x43E;&#x43D;&c_je;&c_t;&#x43A;&c_a;</trd>,
    <trd>&#x43B;&#x44F;&#x43B;&c_je;&c_ch;&#x43A;&c_a; (&#x43A;&#x43E;&#x43C;&c_a;&c_h;&c_i;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x86F9; <pr>y&#x1D2;ng</pr></trd>,
    <trd>&#x4EBA;&#x5076; <pr>r&#xE9;n&#x1D2;u</pr></trd>,
    <trd>&#x5080; <pr>ku&#x1D0;</pr></trd>,
    <trd>&#x6728;&#x5076; <pr>m&#xF9;&#x1D2;u</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pup.gant0o">
  <kap>gant<tld/>o</kap>
  <snc mrk="pup.gant0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="super" cel="marion.0o.TEA">Marioneto</ref> movata per
      en&scirc;ovita mano &ndash; la montra fingro movas &gcirc;ian kapon,
      la dika kaj la longa fingroj movas &gcirc;iajn brakojn:
      <ekz>
        Gant<tld/>o estas en grandeco de &ccirc;. 40 cm kun malgranda
        kapo ligita kun ganto
        <fnt><bib>EncCxi</bib> <lok>&ccirc;apitro
        &leftquot;Pupteatro&rightquot;, p. 234a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_mol;&c_ch;&c_a;&c_t;&c_k;&c_a;&c_v;&c_a;&c_ja; <ind>&c_l;&c_ja;&c_l;&c_mol;&c_k;&c_a;</ind></trd>
  <trd lng="bg">&c_k;&c_u;&c_k;&c_l;&c_a; &c_n;&c_a; &c_r;&c_malmol;&c_k;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>putxinel&middot;li</trd>,
    <trd><ind>titella</ind> de guant</trd>
  </trdgrp>
  <trd lng="de">Handpuppe</trd>
  <trdgrp lng="en">
    <trd>hand puppet</trd>,
    <trd>glove puppet</trd>
  </trdgrp>
  <trd lng="fr">marionnette &agrave; gaine</trd>
  <trdgrp lng="hu">
    <trd>k&eacute;zib&aacute;b</trd>,
    <trd>keszty&udblac;s <ind>b&aacute;b</ind></trd>
  </trdgrp>
  <trd lng="id"><ind>boneka</ind> tangan</trd>
  <trd lng="nl">handpop</trd>
  <trdgrp lng="pt">
    <trd>marionete de luva</trd>,
    <trd>fantoche</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_r;&c_h;&c_o;&c_v;&c_a;&c_ja; <ind>&c_k;&c_u;&c_k;&c_l;&c_a;</ind></trd>,
    <trd>&c_p;&c_je;&c_r;&c_ch;&c_a;&c_t;&c_o;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_k;&c_u;&c_k;&c_l;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="uk">&#x43B;&#x44F;&#x43B;&#x44C;&#x43A;&c_a;</trd>
  <trd lng="zh">&#x5080; <pr>ku&#x1D0;</pr></trd>
</drv>

<drv mrk="pup.faden0o">
  <kap>faden<tld/>o</kap>
  <snc mrk="pup.faden0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="super" cel="marion.0o.TEA">Marioneto</ref>
      movata de supre
      per fadenoj ligitaj al korpo kaj membroj:
      <ekz>
        la faden<tld/>a teatro altiras spektantojn <klr>...</klr>
        per ritmaj paroloj, kantoj, muzika&jcirc;oj kaj dekoracioj
        <fnt><bib>EncCxi</bib>
        <lok>&ccirc;apitro &leftquot;Pupteatro&rightquot;, p. 232a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_y;&c_ja;&c_n;&c_e;&c_t;&c_k;&c_a; <klr>(&c_n;&c_a; &c_n;&c_ib;&c_t;&c_k;&c_a;&c_h;)</klr></trd>
  <trdgrp lng="ca">
    <trd>marioneta</trd>,
    <trd><ind>titella</ind> de fils</trd>
  </trdgrp>
  <trd lng="de">Marionette</trd>
  <trd lng="en">marionette</trd>
  <trdgrp lng="fr">
    <trd>fantoche</trd>
    <trd>marionnette &agrave; fils</trd>,
    <trd>pantin</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>zsin&oacute;ros <ind>b&aacute;b</ind></trd>,
    <trd>marionett</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>boneka</ind> tali</trd>,
    <trd>marionet</trd>
  </trdgrp>
  <trd lng="nl">marionet</trd>
  <trdgrp lng="pt">
    <trd>marionete</trd>,
    <trd>marionete de fio</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_r;&c_i;&c_o;&c_n;&c_je;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_u;&c_k;&c_l;&c_a; &c_n;&c_a; &c_n;&c_i;&c_t;&c_k;&c_a;&c_h;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5080;&#x5121; <pr>ku&#x1D0;l&#x11B;i</pr></trd>,
    <trd>&#x6728;&#x5076; <pr>m&#xF9;&#x1D2;u</pr></trd>,
    <trd>&#x63D0;&#x7DDA;&#x6728;&#x5076; <pr>t&#xED;xi&#xE0;nm&#xF9;&#x1D2;u</pr></trd>,
    <trd>&#x63D0;&#x7EBF;&#x6728;&#x5076; <pr>t&#xED;xi&#xE0;nm&#xF9;&#x1D2;u</pr></trd>,
    <trd>&#x5080; <pr>ku&#x1D0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pup.stango0o">
  <kap>stango<tld/>o</kap>
  <snc mrk="pup.stango0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Granda <ref tip="super" cel="marion.0o.TEA">marioneto</ref>
      movata  de malsupre per stangoj subtenantaj la membrojn:
      <ekz>
        Stanga kaj ferstanga <tld/>oj estas tenataj de ludantoj dum
        prezentado
        <fnt><bib>EncCxi</bib>
        <lok>&ccirc;apitro &leftquot;Pupteatro&rightquot;, p. 232a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_t;&c_o;&c_k;&c_a;&c_v;&c_a;&c_ja; <ind>&c_m;&c_a;&c_r;&c_y;&c_ja;&c_n;&c_e;&c_t;&c_k;&c_a;</ind></trd>,
    <trd>&c_sh;&c_t;&c_o;&c_k;&c_a;&c_v;&c_a;&c_ja; <ind>&c_l;&c_ja;&c_l;&c_mol;&c_k;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="ca"><ind>titella</ind> de varetes</trd>
  <trd lng="de">Stabfigur</trd>
  <trd lng="en">rod puppet</trd>
  <trd lng="fr">marionnette &agrave; tige</trd>
  <trdgrp lng="hu">
    <trd>botp&aacute;lc&aacute;s <ind>b&aacute;b</ind></trd>,
    <trd>r&uacute;dp&aacute;lc&aacute;s <ind>b&aacute;b</ind></trd>
  </trdgrp>
  <trd lng="id">wayang</trd>
  <trd lng="nl">stokpop</trd>
  <trdgrp lng="pt">
    <trd>marionete</trd>,
    <trd>marionete de vareta</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_o;&c_s;&c_t;&c_je;&c_v;&c_a;&c_ja; <ind>&c_k;&c_u;&c_k;&c_l;&c_a;</ind></trd>
</drv>
</art>
