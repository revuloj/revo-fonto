<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pup.xml,v 1.51 2025/09/05 08:37:43 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pup</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="pup.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pup.0o.ludilo">
  <uzo tip="fak">SPO</uzo>
    <dif>
      Figureto de homo, eventuale de besto, el diversa malpeza materialo, kutime kun moveblaj
      membroj,
      uzata kiel infana <ref tip="lst" cel="lud.0ilo" lst="voko:ludiloj">ludilo</ref>, teatra
      rolulo,
      a&ubreve; sor&ccirc;ilo, por ekspozicio de vestoj ktp:
      <ekz>
        la infano ser&ccirc;is sian <tld/>on
        <fnt><bib>F</bib>, <lok>&FE; 18</lok></fnt>;
      </ekz>
      <ekz>
        en la <tld/>lito ku&scirc;is &scirc;ia <tld/>o Sofio
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        flikkudrita <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;parmonujo</lok>
        </fnt>;
      </ekz>
      <ekz>
        flava vaksa <tld/>o kun la granda nigra &ccirc;apelo
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        lerte faritaj viza&gcirc;oj de vestvendejaj <tld/>oj
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        pupoj <klr>[&#8230;]</klr> el porcelano
        <fnt>
          <bib>Kon11</bib>
          <lok>Pupekspozicio</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <nom>Barbie</nom>-<tld/>oj speciali&gcirc;is pri metioj kaj ekesprimis la idealojn de moderna
        virino
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2011/010309.php"
              >Ludilmuzeo de Helsinko</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;i ne suspektis, ke <klr>[&#8230;]</klr> &scirc;i estis nur <tld/>o, kiun li vestis per sia
        revo
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Sepa</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        elegantulino, nura <tld/>o<fnt>B</fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="marion.0o">marioneto</ref>
    <ref tip="vid" cel="teatr.pup0o"><tld/>teatro</ref>
    <trdgrp lng="be">
      <trd>&c_l;&c_ja;&c_l;&c_mol;&c_k;&c_a;</trd>,
      <trd>&c_m;&c_a;&c_r;&c_y;&c_ja;&c_n;&c_e;&c_t;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="bg">&c_k;&c_u;&c_k;&c_l;&c_a;</trd>
    <trdgrp lng="ca">
      <trd>nina</trd>,
      <trd>ninot</trd>,
      <trd>titella</trd>
    </trdgrp>
    <trd lng="cv">&c_p;&c_u;&c_k;&c_a;&c_n;&c_je;</trd>
    <trdgrp lng="de">
      <trd>Puppe</trd>,
      <trd>Spielzeugpuppe</trd>
    </trdgrp>
    <trdgrp lng="en"><trd>doll</trd>, <trd>puppet</trd></trdgrp>
    <trdgrp lng="es">
      <trd>mu&ntilde;eca<klr tip="amb"> (juguete)</klr></trd>,
      <trd>marioneta</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>marionnette</trd>,
      <trd>poup&eacute;e</trd>
    </trdgrp>
    <trd lng="hu">baba</trd>
    <trd lng="id">boneka</trd>
    <trdgrp lng="pt">
      <trd>boneco</trd>,
      <trd>boneca</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_u;&c_k;&c_l;&c_a;</trd>,
      <trd>&c_m;&c_a;&c_r;&c_i;&c_o;&c_n;&c_je;&c_t;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="sv">docka</trd>
    <trdgrp lng="zh">
    <trd>&#29609;&#20598; <pr>w&aacute;n&#466;u</pr></trd>,
    <trd>&#23043;&#23043; <pr>w&aacute;wa</pr></trd>,
    <trd>&#27915;&#23043;&#23043; <pr>y&aacute;ngw&aacute;wa</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="pup.0o.ZOO">
    <uzo tip="stl">KOMUNE</uzo>
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="krizal.0o.ZOO">krizalido</ref>
        <trdgrp lng="be">
      <trd>&c_l;&c_ja;&c_l;&c_je;&c_ch;&c_k;&c_a; <klr tip="amb">(&c_z;&c_a;&c_a;&c_l;.)</klr></trd>,
      <trd>&c_k;&c_u;&c_k;&c_a;&c_l;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="ca">pupa</trd>
    <trdgrp lng="de">
      <trd>Puppe</trd>,
      <trd>Schmetterlingspuppe</trd>
    </trdgrp>
    <trd lng="fr">chrysalide</trd>
    <trd lng="hu">b&aacute;b</trd>
    <trdgrp lng="id">
      <trd>kepompong</trd>,
      <trd>pupa</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>pupa</trd>,
      <trd>cris&aacute;lida</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_u;&c_k;&c_o;&c_l;&c_k;&c_a;</trd>
  </snc>
  <trd lng="bo">&t_a;&t_tsheg;&t_la;&t_sa;&t_tsheg;&t_pa;&t_da;&t_tsheg;&t_ka;&t_naro;&t_ga;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>loutka</trd>,
    <trd>panenka</trd>,
    <trd>panenka <klr>(hra&ccaron;ka)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>chrysalis</trd>,
    <trd>pupa</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#20154;&#24418;  <pr>&#12402;&#12392;&#12364;&#12383;</pr></trd>,
    <trd>&#25351;&#20154;&#24418;  <pr>&#12422;&#12403;&#12395;&#12435;&#12366;&#12423;&#12358;</pr></trd>,
    <trd>&#12373;&#12394;&#12366;</trd>,
    <trd>&#12362;&#20154;&#24418;  <pr>&#12362;&#12395;&#12435;&#12366;&#12423;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">pop</trd>
  <trdgrp lng="sk">
    <trd>b&aacute;bika</trd>,
    <trd>panenka <klr>(hra&ccaron;ka)</klr></trd>
  </trdgrp>
  <trd lng="sv">puppa</trd>
  <trdgrp lng="uk">
    <trd>&c_l;&c_ja;&c_l;&c_mol;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_ib;&c_o;&c_n;&c_je;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_l;&c_ja;&c_l;&c_je;&c_ch;&c_k;&c_a; (&c_k;&c_o;&c_m;&c_a;&c_h;&c_i;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#34553; <pr>y&#466;ng</pr></trd>,
    <trd>&#20154;&#20598; <pr>r&eacute;n&#466;u</pr></trd>,
    <trd>&#20608; <pr>ku&#464;</pr></trd>,
    <trd>&#26408;&#20598; <pr>m&ugrave;&#466;u</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pup.0isto">
  <kap>
    <tld/>isto, <var><kap><tld/>ludisto</kap></var>
  </kap>
  <snc mrk="pup.0isto.prezentisto">
    <dif>
      Prezentisto de <tld/>teatra&jcirc;oj:
      <ekz>
        sian tutan personaron li portis <klr>[&#8230;]</klr> en granda kesto, li estis
        <tld/>ludisto
        <fnt>
          <bib>Fab4</bib>
          <lok>Pupludisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>istoj estas alvokataj por prezenti ombroludon
        <fnt>
          <bib>OIP</bib>
          <lok>Ombro sur interna pejza&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>istoj faris <tld/>ajn kapojn kaj membrojn, kaj dekoristoj konstruis la
        scenojn
        <fnt>
          <bib>Monato</bib>
          <aut>Hayasi Kaneyuki</aut>
          <vrk><url ref="&Monato;/2006/008846.php"
              >Priploru la krizan temon</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni <tld/>istoj celas eduki la infanojn, distrante ilin
        <fnt>
          <bib>Monato</bib>
          <aut>Julia Sigmond</aut>
          <vrk><url ref="&Monato;/2011/010147.php"
              >Georgo</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_l;&c_je;&c_ch;&c_n;&c_ib;&c_k; <klr tip="amb">(&c_a;&c_k;&c_c;&c_jo;&c_r;)</klr></trd>
  <trd lng="de">Puppenspieler</trd>
  <trd lng="en">puppet player</trd>
  <trd lng="fr">montreur de marionnettes</trd>
</drv>

<drv mrk="pup.gant0o">
  <kap>gant<tld/>o</kap>
  <snc mrk="pup.gant0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="super" cel="marion.0o.TEA">Marioneto</ref> movata per
      en&scirc;ovita mano &ndash; la montra fingro movas &gcirc;ian kapon,
      la dika kaj la longa fingroj movas &gcirc;iajn brakojn:
      <ekz>
        gant<tld/>o estas en grandeco de &ccirc;. 40 cm kun malgranda
        kapo ligita kun ganto
        <fnt><bib>EncCxi</bib> <lok>&ccirc;apitro
        &leftquot;Pupteatro&rightquot;, p. 234a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_mol;&c_ch;&c_a;&c_t;&c_k;&c_a;&c_v;&c_a;&c_ja; <ind>&c_l;&c_ja;&c_l;&c_mol;&c_k;&c_a;</ind></trd>
  <trd lng="bg">&c_k;&c_u;&c_k;&c_l;&c_a; &c_n;&c_a; &c_r;&c_malmol;&c_k;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>putxinel&middot;li</trd>,
    <trd><ind>titella</ind> de guant</trd>
  </trdgrp>
  <trd lng="de">Handpuppe</trd>
  <trdgrp lng="en">
    <trd>hand puppet</trd>,
    <trd>glove puppet</trd>
  </trdgrp>
  <trd lng="fr">marionnette &agrave; gaine</trd>
  <trdgrp lng="hu">
    <trd>k&eacute;zib&aacute;b</trd>,
    <trd>keszty&udblac;s <ind>b&aacute;b</ind></trd>
  </trdgrp>
  <trd lng="id"><ind>boneka</ind> tangan</trd>
  <trd lng="nl">handpop</trd>
  <trdgrp lng="pt">
    <trd>marionete de luva</trd>,
    <trd>fantoche</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_r;&c_h;&c_o;&c_v;&c_a;&c_ja; <ind>&c_k;&c_u;&c_k;&c_l;&c_a;</ind></trd>,
    <trd>&c_p;&c_je;&c_r;&c_ch;&c_a;&c_t;&c_o;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_k;&c_u;&c_k;&c_l;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="uk">&c_l;&c_ja;&c_l;&c_mol;&c_k;&c_a;</trd>
  <trd lng="zh">&#20608; <pr>ku&#464;</pr></trd>
</drv>

<drv mrk="pup.faden0o">
  <kap>faden<tld/>o</kap>
  <snc mrk="pup.faden0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="super" cel="marion.0o.TEA">Marioneto</ref>
      movata de supre
      per fadenoj ligitaj al korpo kaj membroj:
      <ekz>
        la faden<tld/>a teatro altiras spektantojn <klr>[&#8230;]</klr>
        per ritmaj paroloj, kantoj, muzika&jcirc;oj kaj dekoracioj
        <fnt><bib>EncCxi</bib>
        <lok>&ccirc;apitro &leftquot;Pupteatro&rightquot;, p. 232a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_y;&c_ja;&c_n;&c_e;&c_t;&c_k;&c_a; <klr>(&c_n;&c_a; &c_n;&c_ib;&c_t;&c_k;&c_a;&c_h;)</klr></trd>
  <trdgrp lng="ca">
    <trd>marioneta</trd>,
    <trd><ind>titella</ind> de fils</trd>
  </trdgrp>
  <trd lng="de">Marionette</trd>
  <trd lng="en">marionette</trd>
  <trdgrp lng="fr">
    <trd>fantoche</trd>
    <trd>marionnette &agrave; fils</trd>,
    <trd>pantin</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>zsin&oacute;ros <ind>b&aacute;b</ind></trd>,
    <trd>marionett</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>boneka</ind> tali</trd>,
    <trd>marionet</trd>
  </trdgrp>
  <trd lng="nl">marionet</trd>
  <trdgrp lng="pt">
    <trd>marionete</trd>,
    <trd>marionete de fio</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_r;&c_i;&c_o;&c_n;&c_je;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_u;&c_k;&c_l;&c_a; &c_n;&c_a; &c_n;&c_i;&c_t;&c_k;&c_a;&c_h;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20608;&#20769; <pr>ku&#464;l&ecaron;i</pr></trd>,
    <trd>&#26408;&#20598; <pr>m&ugrave;&#466;u</pr></trd>,
    <trd>&#25552;&#32218;&#26408;&#20598; <pr>t&iacute;xi&agrave;nm&ugrave;&#466;u</pr></trd>,
    <trd>&#25552;&#32447;&#26408;&#20598; <pr>t&iacute;xi&agrave;nm&ugrave;&#466;u</pr></trd>,
    <trd>&#20608; <pr>ku&#464;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pup.stango0o">
  <kap>stango<tld/>o</kap>
  <snc mrk="pup.stango0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Granda <ref tip="super" cel="marion.0o.TEA">marioneto</ref>
      movata  de malsupre per stangoj subtenantaj la membrojn:
      <ekz>
        stanga kaj ferstanga <tld/>oj estas tenataj de ludantoj dum
        prezentado
        <fnt><bib>EncCxi</bib>
        <lok>&ccirc;apitro &leftquot;Pupteatro&rightquot;, p. 232a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_t;&c_o;&c_k;&c_a;&c_v;&c_a;&c_ja; <ind>&c_m;&c_a;&c_r;&c_y;&c_ja;&c_n;&c_e;&c_t;&c_k;&c_a;</ind></trd>,
    <trd>&c_sh;&c_t;&c_o;&c_k;&c_a;&c_v;&c_a;&c_ja; <ind>&c_l;&c_ja;&c_l;&c_mol;&c_k;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="ca"><ind>titella</ind> de varetes</trd>
  <trd lng="de">Stabfigur</trd>
  <trd lng="en">rod puppet</trd>
  <trd lng="fr">marionnette &agrave; tige</trd>
  <trdgrp lng="hu">
    <trd>botp&aacute;lc&aacute;s <ind>b&aacute;b</ind></trd>,
    <trd>r&uacute;dp&aacute;lc&aacute;s <ind>b&aacute;b</ind></trd>
  </trdgrp>
  <trd lng="id">wayang</trd>
  <trd lng="nl">stokpop</trd>
  <trdgrp lng="pt">
    <trd>marionete</trd>,
    <trd>marionete de vareta</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_o;&c_s;&c_t;&c_je;&c_v;&c_a;&c_ja; <ind>&c_k;&c_u;&c_k;&c_l;&c_a;</ind></trd>
</drv>
