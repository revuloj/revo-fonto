<drv mrk="inteli.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="inteli.0ulo.KOMUNE">
    <dif>
      Homo <tld/>a:
      <ekz>
        se oni observas la homojn, oni konstatas, ke inter ili
        trovi&gcirc;as
        malmulte da <tld/>uloj: preska&ubreve; &ccirc;iuj ja estas
        kutimuloj,
        rutinuloj, tradiciuloj
        <fnt><bib>Lanti</bib><lok>Pensoj</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_z;&c_u;&c_m;&c_n;&c_y; &c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;</trd>
    <trd lng="de">intelligenter Mensch</trd>
    <trd lng="en">intelligent person</trd>
    <trdgrp lng="hu">
      <trd>intelligens ember</trd>,
      <trd>&eacute;rtelmes ember</trd>
    </trdgrp>
    <trd lng="pl">inteligent</trd>
    <trd lng="pt">pessoa <ind>inteligente</ind></trd>
    <trd lng="zh">&#30693;&#35672;&#20998;&#23376; <pr>zh&#299;shif&egrave;nz&#464;</pr></trd>
</snc>
  <snc mrk="inteli.0ulo.intelektulo">
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="sin" cel="intele.0ulo">Intelektulo</ref>:
      <ekz>
        mi, kiel &ccirc;iu <tld/>ulo, ne povas verki sen la humoro
        <fnt><bib>Metrop</bib>, <lok>&ccirc;apitro 1a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_ib;&c_n;&c_t;&c_e;&c_l;&c_ib;&c_g;&c_je;&c_n;&c_t;</trd>,
      <trd>&c_ib;&c_n;&c_t;&c_e;&c_l;&c_je;&c_k;&c_t;&c_u;&c_a;&c_l;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Intellektueller</trd>,
      <trd>Intellektuelle</trd>
    </trdgrp>
    <trd lng="en">intellectual</trd>
    <trdgrp lng="hu">
      <trd>entellekt&uuml;el</trd>,
      <trd>&eacute;rtelmis&eacute;gi ember</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>inteligent</trd>,
      <trd>intelektualista</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#30693;&#35672;&#20998;&#23376; <pr>zh&#299;shif&egrave;nz&#464;</pr></trd>,
    <trd>&#35712;&#26360;&#20154; <pr>d&uacute;sh&#363;r&eacute;n</pr></trd>
  </trdgrp>
</snc>
  <trd lng="ca">persona <ind>intel&middot;ligent</ind></trd>
  <trdgrp lng="cs">
    <trd>inteligent</trd>,
    <trd>vzd&ecaron;lanec</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#25165;&#20154;  <pr>&#12373;&#12356;&#12376;&#12435;</pr></trd>,
    <trd>&#33521;&#25165;  <pr>&#12360;&#12356;&#12373;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>inteligent</trd>,
    <trd>vzdelanec</trd>
  </trdgrp>
</drv>

