<drv mrk="inteli.0a">
  <kap><tld/>a</kap>
  <snc mrk="inteli.0a.PSI">
    <dif>
      Facile komprenanta kaj vigle pensanta:
      <ekz>
        <tld/>a infano, lernanto, servisto;
      </ekz>
      <ekz>
        li ne estas tre <tld/>a rilate al sia a&gcirc;o;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>aj rigardoj, respondoj;
      </ekz>
      <ekz>
        dekdujara knabineto, ridanta, brilanta per siaj <tld/>aj okuloj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        homo <tld/>a kaj akiranta sian panon per laboro spirita
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la policestro estis homo <tld/>a: li tuj divenis, kiu estis la
        mortiginto
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">ARK</uzo>
        <uzo tip="stl">FIG</uzo>
        la plimulto de la <tld/>a mondo <klr>(de la monda
        <ref cel="intele.0ularo">intelektularo</ref>)</klr>
        jam nun trovos tiun &ccirc;i demandon tute superflua
        <fnt><bib>EE</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pruden.0a">prudenta</ref>,
      <ref cel="sagac.0a">sagaca</ref>,
      <ref cel="sagx.0a">sa&gcirc;a</ref>,
      <ref cel="sprit.0a">sprita</ref>,
      <ref cel="ruz.0a">ruza</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_u;&c_m;&c_n;&c_y;</trd>,
    <trd>&c_k;&c_je;&c_m;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_k;&c_je;&c_m;&c_k;&c_ib;</trd>,
    <trd>&c_k;&c_je;&c_m;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bo">&t_ra;&t_gigu;&t_ga;&t_tsheg;&t_pa;&t_tsheg;&t_ca;&t_na;&t_tsheg;</trd>
  <trd lng="ca">intel&middot;ligent</trd>
  <trdgrp lng="cs">
    <trd>ch&aacute;pav&yacute;</trd>,
    <trd>inteligentn&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>intelligent</trd>,
    <trd>klug</trd>,
    <trd>schlau</trd>,
    <trd>clever</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>intelligent</trd>,
    <trd>smart</trd>,
    <trd>clever</trd>,
    <trd>bright</trd>
  </trdgrp>
  <trd lng="es">inteligente</trd>
  <trdgrp lng="fr">
    <trd>intelligent</trd>,
    <trd>malin</trd>,
    <trd>ing&eacute;nieux</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>intelligens</trd>,
    <trd>&eacute;rtelmes</trd>,
    <trd>j&oacute;
    <ind>felfog&oacute;k&eacute;pess&eacute;g&udblac;</ind></trd>
  </trdgrp>
  <trd lng="io">inteligenta</trd>
  <trdgrp lng="ja">
    <trd>&#32865;&#26126;&#12394;  <pr>&#12381;&#12358;&#12417;&#12356;&#12394;</pr></trd>,
    <trd>&#38957;&#12398;&#12424;&#12356;  <pr>&#12354;&#12383;&#12414;&#12398;&#12356;&#12356;</pr></trd>,
    <trd>&#21033;&#21475;&#12394;  <pr>&#12426;&#12371;&#12358;&#12394;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>intelligent</trd>,
    <trd>slim</trd>,
    <trd>pienter</trd>
  </trdgrp>
  <trd lng="pl">inteligentny</trd>
