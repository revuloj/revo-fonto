<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>agrabl</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="agrabl.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="agrabl.0a.KOMUNE">
    <dif>
      Kiu pla&ccirc;as kaj plezurigas:
      <ekz>
        la viza&gcirc;o de tiu infano estas <tld/>a;
      </ekz>
      <ekz>
        lia vizito estis al mi tre <tld/>a;
      </ekz>
      <ekz>
        pli bona estas vorto afabla ol kuko
        <tld/>a<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        sento de <tld/>eco<fnt>Z</fnt>;
      </ekz>
      <ekz>
        estas al mi tre <tld/>e, ke mi havas nun la okazon saluti
        vin<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_je;&c_m;&c_n;&c_y;</trd>
  <trd lng="br">plijus</trd>
  <trd lng="ca">agradable</trd>
  <trdgrp lng="cs">
    <trd>lahodn&yacute;</trd>,
    <trd>mil&yacute;</trd>,
    <trd>p&rcaron;&iacute;jemn&yacute;</trd>,
    <trd>p&rcaron;&iacute;jemn&yacute;</trd>
  </trdgrp>
  <trd lng="de">angenehm</trd>
  <trd lng="en">pleasant</trd>
  <trd lng="es">agradable</trd>
  <trd lng="fr">agr&eacute;able</trd>
  <trd lng="hu">kellemes</trd>
  <trd lng="it">gradevole</trd>
  <trdgrp lng="nl">
    <trd>aangenaam</trd>,
    <trd>prettig</trd>
  </trdgrp>
  <trd lng="oc">agradable</trd>
  <trdgrp lng="pl">
    <trd>przyjemny</trd>,
    <trd>mi&lstroke;y</trd>,
    <trd>b&lstroke;ogi</trd>,
    <trd>fajny</trd>,
    <trd>luby<klr> (przestarz.)</klr></trd>,
    <trd>&lstroke;adny</trd>,
    <trd>odpr&eogonek;&zdot;aj&aogonek;cy</trd>,
    <trd>powabny</trd>,
    <trd>przytulny</trd>,
    <trd>rozkoszny</trd>,
    <trd>sympatyczny</trd>
    <trd>wdzi&eogonek;czny</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>agrad&aacute;vel</trd>,
    <trd>ameno</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_ja;&c_t;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>lahodn&yacute;</trd>,
    <trd>pr&iacute;jemn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>trevlig</trd>,
    <trd>angen&auml;m</trd>
  </trdgrp>
  <trd lng="tr">ho&scedil;</trd>
</drv>

<drv mrk="agrabl.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="agrabl.0ajxo.KOMUNE">
    <dif>
      Io <tld/>a, plezuriga:
      <ekz>
        la re&gcirc;ido &ccirc;iam restis apud &scirc;i kaj diris al &scirc;i multajn
        <tld/>a&jcirc;ojn
        <fnt>
          <aut>C. Perrault, trad. P. Sarpy</aut>,
          <vrk><url
          ref="https://eo.wikisource.org/wiki/Rakontoj_pri_Feinoj/Cindrulino_a%C5%AD_la_vitra_Pantofleto">
          Cindrulino</url></vrk>,
          <lok>en: Rakontoj pri Feinoj, 1904</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>agr&eacute;ment</trd>,
    <trd>avantage</trd>,
    <trd>chose <ind>agr&eacute;able</ind></trd>,
    <trd>plaisir</trd>
  </trdgrp>
</drv>

<drv mrk="agrabl.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="agrabl.mal0a.KOMUNE">
    <dif>
      Malplezuriga, malpla&ccirc;a, teda, koleretiga:
      <ekz>
        mal<tld/>a odoro, vo&ccirc;o, kuko, renkonto.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="abomen.0a">abomena</ref>,
      <ref cel="nauxz">na&ubreve;za</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_p;&c_r;&c_y;&c_je;&c_m;&c_n;&c_y;</trd>
  <trd lng="br">displijus</trd>
  <trd lng="ca">desagradable</trd>
  <trdgrp lng="cs">
    <trd>nep&rcaron;&iacute;jemn&yacute;</trd>,
    <trd>nep&rcaron;&iacute;jemn&yacute;</trd>,
    <trd>protivn&yacute;</trd>
  </trdgrp>
  <trd lng="de">unangenehm</trd>
  <trd lng="en">unpleasant</trd>
  <trd lng="es">desagradable</trd>
  <trd lng="fr">d&eacute;sagr&eacute;able</trd>
  <trd lng="hu">kellemetlen</trd>
  <trd lng="it">sgradevole</trd>
  <trd lng="nl">onaangenaam</trd>
  <trd lng="oc">desagradable</trd>
  <trdgrp lng="pl">
    <trd>nieprzyjemny</trd>,
    <trd>niemi&lstroke;y</trd>,
    <trd>niefajny</trd>,
    <trd>niezno&sacute;ny</trd>,
    <trd>k&lstroke;opotliwy</trd>,
    <trd>niesympatyczny</trd>,
    <trd>niewdzi&eogonek;czny</trd>,
    <trd>odpychaj&aogonek;cy</trd>,
    <trd>odstr&eogonek;czaj&aogonek;cy</trd>,
    <trd>paskudny</trd>,
    <trd>przykry</trd>
  </trdgrp>
  <trd lng="pt">desagrad&aacute;vel</trd>
  <trd lng="ru">
    &c_n;&c_je;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_n;&c_y;&c_j;
  </trd>
  <trd lng="sk">nepr&iacute;jemn&yacute;</trd>
  <trd lng="sv">otrevlig</trd>
  <trdgrp lng="tr">
    <trd>tats&inodot;z</trd>,
    <trd>ho&scedil; olmayan</trd>
  </trdgrp>
</drv>

<drv mrk="agrabl.mal0ajxo">
  <kap>mal<tld/>a&jcirc;o</kap>
  <snc mrk="agrabl.mal0ajxo.KOMUNE">
    <dif>
      Io ne <tld/>a, &gcirc;ena:
      <ekz>
        &ccirc;iu profesio havas siajn mal<tld/>a&jcirc;ojn
        <fnt>
          <aut>B. N&ecaron;mcov&aacute;, trad. V. Tobek kaj K. Proch&aacute;zka</aut>,
          <vrk><url ref="https://eo.wikisource.org/wiki/Avineto/XI.">
          Avineto</url></vrk>,
          <lok>[sen dato]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>chose <ind>d&eacute;sagr&eacute;able</ind></trd>,
    <trd>d&eacute;plaisir</trd>,
    <trd>d&eacute;sagr&eacute;ment</trd>,
    <trd>inconv&eacute;nient</trd>
  </trdgrp>
</drv>
</art>
