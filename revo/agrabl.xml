<drv mrk="agrabl.mal0ajxo">
  <kap>mal<tld/>a&jcirc;o</kap>
  <snc mrk="agrabl.mal0ajxo.KOMUNE">
    <dif>
      Io ne <tld/>a, &gcirc;ena:
      <ekz>
        <klr>[li]</klr> atingis altan honoron <klr>[&#x2026;]</klr> nur post longaj kaj multaj
        mal<tld/>a&jcirc;oj kaj dan&gcirc;eroj en sia vivo
        <fnt>
          <bib>Fab3</bib>
          <lok>Dorna vojo de la honoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        li aludas al mi pri ia mal<tld/>a&jcirc;o
        <fnt>
          <bib>Fab4</bib>
          <lok>Ne&gcirc;ulo</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi pacience eltenis en la da&ubreve;ro de dudek jaroj, malgra&ubreve; la multaj atakoj kaj
        mal<tld/>a&jcirc;oj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Tria Kongreso Esperantista en Cambridge en la 12a de a&ubreve;gusto 1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        ne faru al vi la mal<tld/>a&jcirc;on rakonti al mi la detalojn
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu profesio havas siajn mal<tld/>a&jcirc;ojn
        <fnt>
          <aut>B. N&ecaron;mcov&aacute;, trad. V. Tobek kaj K.
          Proch&aacute;zka</aut>,
          <vrk><url ref="https://eo.wikisource.org/wiki/Avineto/XI.">
          Avineto</url></vrk>,
          <lok>[sen dato]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x4E0D;&#x65B9;&#x4FBF; <pr>b&#xF9;f&#x101;ngbi&#xE0;n</pr></trd>
</snc>
  <trdgrp lng="de">
    <trd>Unannehmlichkeit</trd>,
    <trd>uanangenehme <ind>Sache</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>chose <ind>d&eacute;sagr&eacute;able</ind></trd>,
    <trd>d&eacute;plaisir</trd>,
    <trd>d&eacute;sagr&eacute;ment</trd>,
    <trd>inconv&eacute;nient</trd>
  </trdgrp>
</drv>
