<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ruteni.xml,v 1.16 2018/11/19 21:10:17 revo Exp $">
<kap>
  <rad>ruteni</rad>/o
</kap>
<drv mrk="ruteni.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Ru</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
      lst="voko:&hcirc;emiaj_elementoj" val="44">elemento</ref> kun
      atomnumero 44, atompezo 101,07;
      transir<ref tip="super" cel="metal.0o">metalo</ref> kun 
      amfotera karaktero:
      <ekz>
        ekzemploj de radiaktivaj nuklidoj <klr>[&#8230;]</klr> <tld/>o <sup>106</sup>Ru
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=33259"
              >Radiaktiveco</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_u;&c_t;&c_e;&c_n;&c_ib;&c_j;</trd>
  <trd lng="br">ruteniom</trd>
  <trdgrp lng="cs">
    <trd>Ru</trd>,
    <trd>ruthenium</trd>
  </trdgrp>
  <trd lng="de">Ruthenium</trd>
  <trd lng="en">ruthenium</trd>
  <trd lng="es">rutenio</trd>
  <trd lng="fr">ruth&eacute;nium</trd>
  <trd lng="hu">rut&eacute;nium</trd>
  <trd lng="id">rutenium</trd>
  <trd lng="it">rutenio</trd>
  <trd lng="la">Ruthenium</trd>
  <trd lng="nl">ruthenium</trd>
  <trd lng="pl">ruten</trd>
  <trdgrp lng="pt">
    <trd>rut&ecirc;nio</trd>,
    <trd>rut&eacute;nio</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_u;&c_t;&c_je;&c_n;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>Ru</trd>,
    <trd>ruth&eacute;nium</trd>
  </trdgrp>
  <trd lng="sv">rutenium</trd>
</drv>
</art>
