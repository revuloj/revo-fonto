<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gaberon.xml,v 1.3 2018/10/02 16:10:14 revo Exp $">
<kap>
  <rad>Gaberon</rad>/o <fnt><bib>PIV3</bib></fnt>,
  <var><kap>Gaboron/o</kap></var>
</kap>
<drv mrk="gaberon.G0o">
  <kap><tld/>o,
    <var><kap>Gaborono</kap></var>
  </kap>
  <snc mrk="gaberon.G0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" cel="urb.cxef0o" lst="voko:&ccirc;efurboj">&Ccirc;efurbo</ref>
      de <ref tip="malprt" cel="bocvan.B0o">Bocvano</ref>:
      <ekz>
        oni translokis en 1965 la registarejon de Bocvano (tiam Be&ccirc;uanalando) de <nom>Mafikeng</nom> al <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=33236">Gaberono</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        Manpilkado-Asocio de Bocvano, sidejo: Gaborono 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=473142">Manpilkado-Asocio de Bocvano</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Gaborone</trd>
  </snc>
  <trd lng="fr">Gaborone</trd>
  <trd lng="nl">Gaborone</trd>
  <trd lng="no">Gaborone</trd>
</drv>
</art>

