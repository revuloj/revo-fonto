<art mrk="$Id: slovaki.xml,v 1.9 2022/03/27 17:06:36 revo Exp $">
<kap>
  <rad>Slovaki</rad>/o
</kap>

<drv mrk="slovaki.S0o">
  <kap>
    <tld/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="slovaki.S0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="slovak.S0ujo">Slovakujo</ref>:
      <ekz>
        Bratislavo, la &ccirc;efurbo de <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2003/008017.html">Fervojo fivoja</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ekde majo 2004 <tld/>o <klr>[&#x2026;]</klr> estos membro de E&ubreve;ropa Unio
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2003/008017.html">Fervojo fivoja</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o evoluas &ccirc;iam pli direkte al a&ubreve;to-eksportlando
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2004/008396.html">Profunda struktur&scirc;an&gcirc;o en meza kaj orienta
          E&ubreve;ropo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        al la ekesto de slovakaj popolaj kostumoj kontribuis <klr>[&#x2026;]</klr> la klimataj
        kaj geografiaj kondi&ccirc;oj en <tld/>o, kiu havas kaj ebena&jcirc;ojn kaj altmontarojn
        <fnt>
          <bib>Monato</bib>
          <aut>Stano Mar&ccaron;ek</aut>
          <vrk><url ref="&Monato;/2010/009986.php">Popolajn kostumojn portas pupoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Slowakei</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_S;&c_l;&c_a;&c_v;&c_a;&c_k;&c_ib;&c_ja;</trd>,
    <trd>&c_S;&c_l;&c_a;&c_v;&c_a;&c_ch;&c_ch;&c_y;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Slovaquie</trd>,
    <trd>R&eacute;publique slovaque</trd>
  </trdgrp>
  <trd lng="id">Slovakia</trd>
  <trdgrp lng="pl">
    <trd>S&lstroke;owacja</trd>,
    <trd>Republika <ind>S&lstroke;owacji</ind></trd>
  </trdgrp>
  <trd lng="uk">&#x421;&#x43B;&#x43E;&#x432;&#x430;&#x447;&#x447;&#x438;&#x43D;&#x430;</trd>
</drv>

</art>

