<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: bunkr.xml,v 1.5 2023/10/29 19:11:04 afido Exp $">
    <kap>
      <rad>bunkr</rad>/o
    </kap>

<drv mrk="bunkr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc mrk="bunkr.0o.sxirmejo">
    <dif>
      Tre fortika &scirc;irmejo, kun muroj el armita betono a&ubreve; natura roko,
      kiajn oni konstruis ekde la 20a jc, 
      unu- a&ubreve; plur&ccirc;ambra, protektanta kontra&ubreve; bombado, dan&gcirc;eraj substancoj, radiadoj kc:
      <ekz>
        <klr>[tie]</klr> troveblas subsurfacaj <tld/>oj kaj tuneloj &ndash; rifu&gcirc;ejoj por la
        armeo kaj por la lo&gcirc;antaro
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2008/009574.php">Lando sur- kaj sub-tera</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Stauffenberg</nom> lasis en <tld/>o, kie la Diktatoro konferencis kun sia stabo,
        bombon ka&scirc;itan en teko
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2011/010353.php">La ultradimensio</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        la belga armeo ekuzis la <tld/>on por instali sekuran komandejon, uzotan dum
        milito
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2010/009873.php">Bunkro remalkovrita</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        li e&ccirc; havis <klr>[&#x2026;]</klr> en sia <tld/>o &ndash; fotolaboratorion, kies
        elektrokurenton liveris akumulatoro
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2005/008463.php">Strangulo en la naturo</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="bastio.0o">bastiono</ref>,
       <ref cel="citade.0o">citadelo</ref>,
       <ref cel="kazema.0o">kazemato</ref>
    </refgrp>
    <trd lng="de">Bunker</trd>
    <trd lng="en">bunker</trd>
    <trd lng="ru">&c_b;&c_u;&c_n;&c_k;&c_je;&c_r;</trd>
    <trdgrp lng="zh">
    <trd>&#x7889;&#x5821; <pr>di&#x101;ob&#x1CE;o</pr></trd>,
    <trd>&#x7164;&#x4ED3; <pr>m&#xE9;ic&#x101;ng</pr></trd>,
    <trd>&#x907F;&#x5F39;&#x5BA4; <pr>b&#xEC;d&#xE0;nsh&#xEC;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_b;&c_u;&c_n;&c_k;&c_je;&c_r; <klr>(&c_u;&c_m;&c_a;&c_c;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;)</klr></trd>
  <trd lng="he">&#x5D1;&#x5BC;&#x5D5;&#x5BC;&#x5E0;&#x5E7;&#x5B6;&#x5E8;</trd>
</drv>

