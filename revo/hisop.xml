<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>hisop</rad>/o
</kap>
<drv mrk="hisop.0o">
  <kap><tld/>o</kap>
  <snc mrk="hisop.0o.BIB">
    <uzo tip="fak">BIB</uzo>
    <dif>
      Nekonata vegeta&jcirc;o, citata en la Biblio kiel tre malgranda kaj
      bonaroma.
    </dif>
  </snc>
  <snc mrk="hisop.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="lamiac.0oj">lamiacoj</ref>
      (<trd lng="la">Hyssopus</trd>), uzata kiel spica&jcirc;o kaj en
      medicino.
    </dif>
  </snc>
  <trd lng="de">Ysop</trd>
  <trd lng="en">hyssop</trd>
  <trd lng="fr">hysope</trd>
  <trd lng="hu">izs&oacute;p</trd>
  <trd lng="nl">hysop</trd>
  <trd lng="pl">hyzop</trd>
  <trd lng="ru">&c_i;&c_s;&c_s;&c_o;&c_p;</trd>
</drv>
</art>
