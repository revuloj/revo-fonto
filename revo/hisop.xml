<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>hisop</rad>/o
</kap>
<drv mrk="hisop.0o">
  <kap><tld/>o</kap>
  <snc mrk="hisop.0o.BIB">
    <uzo tip="fak">BIB</uzo>
    <dif>
      Nekonata vegeta&jcirc;o, citata en la Biblio kiel tre malgranda kaj
      bonaroma.
    </dif>
  </snc>
  <snc mrk="hisop.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="lami.0acoj">lamiacoj</ref>
      (<trd lng="la">Hyssopus</trd>), uzata kiel spica&jcirc;o kaj en medicino.
    </dif>
  </snc>
  <trd lng="de">Ysop</trd>
</drv>
</art>
