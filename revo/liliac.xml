<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: liliac.xml,v 1.14 2023/09/01 19:10:59 revo Exp $">
<kap>
  <rad>liliac</rad>/oj
</kap>
<drv mrk="liliac.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref>
      de <ref tip="malprt" cel="kotile.unu0aj">unukotiledonaj</ref>
      plantoj (<trd lng="la">Liliaceae</trd>):
      <ekz>
        la KAM-me&hcirc;anismo estas pruvita &ccirc;e 25 plantaj familioj,
        <klr>[&#x2026;]</klr> krom la
        nomdonanta familio de krasulacoj pliaj familioj estas: kaktacoj,
        <klr>[&#x2026;]</klr> vitacoj, <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=279549">KAM-fotosintezo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
       <ref cel="aspara.0o">asparago</ref>,
       <ref cel="agav.0o">agavo</ref>,
       <ref cel="lili.0o">lilio</ref>,
       <ref cel="tulip.0o">tulipo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_l;&c_je;&c_j;&c_n;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">lili&agrave;cies</trd>
  <trd lng="de">Liliengew&auml;chse</trd>
  <trd lng="el">&kappa;&rho;&epsilon;&jota;&ny;&omikron;&epsilon;&jota;&delta;&eta_ton;</trd>
  <trd lng="fa">&a_s;&a_w;&a_s;&a_n;&f_y;&a_A;&a_n;</trd>
  <trd lng="hu">liliomf&eacute;l&eacute;k</trd>
  <trd lng="ja">&#x30E6;&#x30EA;&#x79D1;  [<ind>&#x30E6;&#x30EA;&#x304B;</ind>]</trd>
  <trd lng="nl">lelieachtigen</trd>
  <trd lng="pl">liliowate</trd>
  <trd lng="pt">lili&aacute;ceas</trd>
  <trd lng="ro">liliaceae</trd>
  <trd lng="ru">&c_l;&c_i;&c_l;&c_je;&c_j;&c_n;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sv">liljev&auml;xter</trd>
  <trd lng="uk">&#x43B;&#x456;&#x43B;&#x456;&#x439;&#x43D;&#x456;</trd>
</drv>
</art>
