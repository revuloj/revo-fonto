<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hidrat.xml,v 1.24 2023/11/15 17:08:00 revo Exp $">
<kap>
  <rad>hidrat</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="hidrat.0o">
  <kap><tld/>o</kap>
  <snc mrk="hidrat.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      &Ccirc;ia substanco, kiu povas esti rigardata kiel
      kombina&jcirc;o de akvo kun alia substanco:
      <ekz>
        esplor-teamo trovis amason de &leftquot;metana <tld/>o&rightquot; sur la fundo de la Japana Maro
        <fnt>
          <vrk><url ref="https://www.vastalto.com/blog/?p=212">Metana hidrato en la Japana Maro -
          Akvosfero</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_d;&c_r;&c_a;&c_t;</trd>
  <trd lng="ca">hidrat</trd>
  <trdgrp lng="cs">
    <trd>hydr&aacute;t</trd>,
    <trd>s&uring;l s krystalickou vodou</trd>
  </trdgrp>
  <trd lng="de">Hydrat</trd>
  <trd lng="el">&ypsilon;&delta;&rho;&omikron;&chi;&epsilon;&jota_ton;&delta;&jota;&omikron;</trd>
  <trd lng="fr">hydrate</trd>
  <trd lng="hu">hidr&aacute;t</trd>
  <trd lng="it">idrato<klr> (sost.)</klr></trd>
  <trd lng="ja">&#x6C34;&#x548C;&#x7269;  <pr>&#x3059;&#x3044;&#x308F;&#x3076;&#x3064;</pr></trd>
  <trd lng="nl">hydraat</trd>
  <trdgrp lng="pl">
    <trd>hydrat</trd>,
    <trd>wodzian</trd>
  </trdgrp>
  <trd lng="pt">hidrato</trd>
  <trd lng="ru">&c_g;&c_i;&c_d;&c_r;&c_a;&c_t;</trd>
  <trd lng="sk">hydr&aacute;t</trd>
  <trd lng="uk">&c_g;&c_ib;&c_d;&c_r;&c_a;&c_t;</trd>
  <trd lng="zh">&#x6C34;&#x5408;&#x7269; <pr>shu&#x1D0;h&#xE9;w&#xF9;</pr></trd>
</drv>

<drv mrk="hidrat.0a">
  <kap><tld/>a</kap>
  <snc mrk="hidrat.0a.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Kombinita kun akvo:
      <ekz>
        <tld/>aj aluminosilikatoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=119">Argilo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_d;&c_r;&c_a;&c_t;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
  <trdgrp lng="ca">
    <trd>hidratat</trd>,
    <trd>aqu&oacute;s</trd>
  </trdgrp>
  <trd lng="de">w&auml;ssrig</trd>
  <trd lng="el">&ypsilon;&delta;&rho;&omikron;&chi;&epsilon;&jota;&delta;&omega;&my;&epsilon_ton;&ny;&omikron;&sigma_fina;</trd>
  <trd lng="fr">hydrat&eacute;<klr tip="ind"> (chim.)</klr></trd>
  <trd lng="hu">vizes</trd>
  <trd lng="it">idrato<klr> (sgg.)</klr></trd>
  <trd lng="ja">&#x6C34;&#x548C;&#x3057;&#x305F;  <pr>&#x3059;&#x3044;&#x308F;&#x3057;&#x305F;</pr></trd>
  <trd lng="nl">waterig</trd>
  <trd lng="pt">hidratado</trd>
  <trd lng="ru">&c_g;&c_i;&c_d;&c_r;&c_a;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  <trd lng="uk">&c_g;&c_ib;&c_d;&c_r;&c_a;&c_t;&#x43D;&c_i;&c_j;</trd>
</drv>

<drv mrk="hidrat.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="hidrat.0igi.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Formi <tld/>on, kombini substancon kun akvo:
      <ekz>
        kalkosulfato <tld/>igita
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=214932">Alabastro</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_d;&c_r;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">hidratar</trd>
  <trdgrp lng="cs">
    <trd>hasit</trd>,
    <trd>hydratovat</trd>,
    <trd>slou&ccaron;it s vodou</trd>
  </trdgrp>
  <trd lng="de">hydratisieren</trd>
  <trd lng="el">&ypsilon;&delta;&rho;&omikron;&chi;&epsilon;&jota;&delta;&omega_ton;&ny;&omega;</trd>
  <trd lng="fr">hydrater<klr tip="ind"> (chim., transformer en
  hydrate)</klr></trd>
  <trd lng="hu">hidr&aacute;t&aacute;l</trd>
  <trd lng="it">idratare</trd>
  <trd lng="ja">&#x6C34;&#x548C;&#x3055;&#x305B;&#x308B;  <pr>&#x3059;&#x3044;&#x308F;&#x3055;&#x305B;&#x308B;</pr></trd>
  <trd lng="nl">hydrateren</trd>
  <trdgrp lng="pl">
    <trd>hydratyzowa&cacute;</trd>, <trd>uwadnia&cacute;</trd>
  </trdgrp>
  <trd lng="pt">hidratar</trd>
  <trd lng="ru">&c_g;&c_i;&c_d;&c_r;&c_a;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>hydr&aacute;tova&tcaron;</trd>,
    <trd>zlu&ccaron;ova&tcaron; s vodou</trd>
  </trdgrp>
</drv>

<drv mrk="hidrat.karbon0o">
  <kap>karbon<tld/>o</kap>
  <snc mrk="hidrat.karbon0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="sin" cel="suker.0o">Sukero</ref> a&ubreve; simila <tld/>a substanco
      funkcianta kiel provizanto de energio al viva&jcirc;oj;
      <ref tip="sin" cel="glucid.0o.KEM">glucido</ref>:
      <ekz>
        karoboj estas ri&ccirc;aj je proteinoj, karbon<tld/>oj <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2011/010535.php">Sanige ... kaj sensukere</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        konsumado de granda kvanto da karbon<tld/>oj en mallonga tempo ka&ubreve;zas rapidan
        kreskon de la sukernivelo en la sango
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van De Velde</aut>
          <vrk><url ref="&Monato;/2013/010934.php">Sablohorlo&gcirc;o for&scirc;ovas piramidon</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        la karbon<tld/>oj estas la fonto de la muskolforto    
        <fnt>
          <aut>A. Wander</aut>,
          <vrk><url ref="http://arkivo.esperanto-france.org/divers/ovomalt/ovo-p5.htm">
          La Plej Esenca pri Ovomaltino</url></vrk>,
          <lok>1910</lok>
        </fnt>;
      </ekz>
      <ekz>
        karbon<tld/>a biopolimero ekstraktata de la hinda guar-arbo
        <fnt>
          <bib>Monato</bib>
          <aut>Werner Fuss</aut>
          <vrk><url ref="&Monato;/2014/011348.php">Novtipa gasminado: &ccirc;u vere dan&gcirc;era?</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_u;&c_g;&c_l;&c_ja;&c_v;&c_o;&c_d;</trd>,
    <trd>&c_s;&c_a;&c_h;&c_a;&c_r;&c_y;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>gl&uacute;cid</trd>,
    <trd>gl&iacute;cid</trd>
  </trdgrp>
  <trd lng="cs">uhlovod&iacute;k</trd>
  <trdgrp lng="de">
    <trd>Kohlenhydrat</trd>,
    <trd>Saccharid</trd>
  </trdgrp>
  <trd lng="en">carbohydrate</trd>
  <trd lng="fr">glucide</trd>
  <trd lng="ja">&#x70AD;&#x6C34;&#x5316;&#x7269;  <pr>&#x305F;&#x3093;&#x3059;&#x3044;&#x304B;&#x3076;&#x3064;</pr></trd>
  <trdgrp lng="pt">
    <trd>glic&iacute;dio</trd>,
    <trd>gluc&iacute;dio</trd>
  </trdgrp>
  <trd lng="sk">uh&lcaron;ovod&iacute;k</trd>
  <trdgrp lng="zh">
    <trd>&#x78B3;&#x6C34;&#x5316;&#x5408;&#x7269; <pr>t&#xE0;nshu&#x1D0;hu&#xE0;h&#xE9;w&#xF9;</pr></trd>,
    <trd>&#x91A3; <pr>t&#xE1;ng</pr></trd>,
    <trd>&#x91A3;&#x7C7B; <pr>t&#xE1;ngl&#xE8;i</pr></trd>,
    <trd>&#x7CD6;&#x7C7B; <pr>t&#xE1;ngl&#xE8;i</pr></trd>
  </trdgrp>
</drv>
</art>
