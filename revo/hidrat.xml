<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hidrat</rad>/o
</kap>
<drv mrk="hidrat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Nomo donata al &ccirc;iu substanco, kiu povas esti konsiderata kiel
      kombina&jcirc;o de akvo kun alia substanco. 
    </dif>
  </snc>
  <trd lng="ru">&c_g;&c_i;&c_d;&c_r;&c_a;&c_t;</trd>
</drv>
<drv mrk="hidrat.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Kombinita kun akvo. 
    </dif>
  </snc>
</drv>
<drv mrk="hidrat.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Formi <tld/>on, kombini substancon kun akvo.  
    </dif>
  </snc>
</drv>
</art>
<!--
$Log$
-->
</vortaro>
