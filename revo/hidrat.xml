<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hidrat</rad>/o
</kap>
<drv mrk="hidrat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Nomo donata al &ccirc;iu substanco, kiu povas esti konsiderata kiel
      kombina&jcirc;o de akvo kun alia substanco.
    </dif>
  </snc>
  <trd lng="de">Hydrat</trd>
  <trd lng="fr">hydrate</trd>
  <trd lng="nl">hydraat</trd>
  <trdgrp lng="pl">
    <trd>hydrat</trd>,
    <trd>wodzian</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_i;&c_d;&c_r;&c_a;&c_t;</trd>
</drv>

<drv mrk="hidrat.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Kombinita kun akvo.
    </dif>
  </snc>
  <trd lng="de">w&auml;ssrig</trd>
  <trd lng="fr">hydrat&eacute;<klr tip="ind"> (chim.)</klr></trd>
  <trd lng="nl">waterig</trd>
</drv>

<drv mrk="hidrat.0igi">
  <kap><tld/>igi</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Formi <tld/>on, kombini substancon kun akvo.
    </dif>
  </snc>
  <trd lng="de">hydratisieren</trd>
  <trd lng="fr">hydrater<klr tip="ind"> (chim., transformer en hydrate)</klr></trd>
  <trd lng="nl">hydrateren</trd>
  <trdgrp lng="pl">
    <trd>hydratowa&cacute;</trd>, <trd>uwadnia&cacute;</trd>
  </trdgrp>
</drv>
</art>
