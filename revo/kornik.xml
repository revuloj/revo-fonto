<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>kornik</rad>/o
</kap>
<drv mrk="kornik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="korv.0o.genro">korvo</ref>
      kaj precipe la orienta griza raso de tiu specio
      (<trd lng="la">Corvus corone cornix</trd>):
      <ekz>
        <ind><tld/>o vundita e&ccirc; propran voston timas</ind><fnt>Z</fnt>.
        <trd lng="nl">door <ind>scha</ind> en schande wordt men
        wijs</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="brog.0i">brogi</ref>,
      <ref cel="spert.0o">sperto</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_r;&c_o;&c_n;&c_a;</trd>
  <trd lng="cs">vr&aacute;na obecn&aacute; &scaron;ed&aacute;</trd>
  <trd lng="de">Nebelkr&auml;he</trd>
  <trd lng="en">hooded crow</trd>
  <trd lng="fr">corneille</trd>
  <trd lng="hu">dolm&aacute;nyos varj&uacute;</trd>
  <trd lng="nl">bonte <ind>kraai</ind></trd>
  <trd lng="pl">wrona siwa</trd>
  <trd lng="pt">gralha</trd>
  <trd lng="ru">&c_s;&c_je;&c_r;&c_a;&c_ja;
  &c_v;&c_o;&c_r;&c_o;&c_n;&c_a;</trd>
</drv>
</art>
