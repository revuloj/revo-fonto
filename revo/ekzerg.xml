<art mrk="$Id: ekzerg.xml,v 1.17 2023/11/07 19:22:37 revo Exp $">
<kap>
  <rad>ekzerg</rad>/o
</kap>
<drv mrk="ekzerg.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      La spaco rezervata sur medaloj, moneroj a&ubreve; simila&jcirc;oj por meti sur &gcirc;i
      surskribon.
    </dif>
    <trd lng="de">Platz f&uuml;r die <ind>Inschrift</ind></trd>
    <trd lng="hu">felirat helye<klr tip="amb"> (&eacute;rm&eacute;n)</klr></trd>
    <trd lng="pt">exergo</trd>
    <trd lng="ru">&c_m;&c_je;&c_s;&c_t;&c_o; &c_d;&c_l;&c_ja; <ind>&c_n;&c_a;&c_d;&c_p;&c_i;&c_s;&c_i;</ind><klr tip="amb"> (&c_n;&c_a; &c_m;&c_je;&c_d;&c_a;&c_l;&c_i;, &c_m;&c_o;&c_n;&c_je;&c_t;&c_je; &c_i; &c_t;.&c_p;.)</klr></trd>
  </snc>
  <snc>
    <dif>
      La <ref tip="super" cel="skrib.sur0o">surskribo</ref> mem:
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> estas surskriba&jcirc;o, indikanta daton, fabriklokon,
        verkistonomon, en la areo limigita per la orlo de monero a&ubreve; medajlo, sub la
        centra figuro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=598205">Ekzergo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="epigra.0o.ARKE">epigrafo<sncref/></ref>
    <trd lng="hu">felirat<klr tip="amb"> (&eacute;rm&eacute;n)</klr></trd>
    <trd lng="pt">exergo</trd>
    <trd lng="ru">&c_n;&c_a;&c_d;&c_p;&c_i;&c_s;&c_mol;<klr tip="amb"> (&c_n;&c_a; &c_m;&c_je;&c_d;&c_a;&c_l;&c_i;, &c_m;&c_o;&c_n;&c_je;&c_t;&c_je; &c_i; &c_t;.&c_p;.)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>h&rcaron;bet</trd>,
    <trd>m&iacute;sto pro n&aacute;pis</trd>,
    <trd>okraj <klr>(mince, medaile)</klr></trd>
  </trdgrp>
  <trd lng="de">Inschrift</trd>
  <trd lng="fr">exergue</trd>
  <trdgrp lng="ja">
    <trd>&#x523B;&#x9298;&#x90E8;  <pr>&#x304D;&#x3056;&#x3081;&#x3044;&#x3076;</pr></trd>,
    <trd>&#x523B;&#x9298;  <pr>&#x3053;&#x304F;&#x3081;&#x3044;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>miesto pre n&aacute;pis <klr>(na minci, medaile)</klr></trd>,
    <trd>miesto pre n&aacute;pis <klr>(na minci| medaile)</klr></trd>
  </trdgrp>
  <trd lng="uk">&#x43C;&#x456;&#x441;&#x446;&#x435; &#x434;&#x43B;&#x44F; &#x43D;&#x430;&#x43F;&#x438;&#x441;&#x443; &#x456; &#x434;&#x430;&#x442;&#x438; &#x43D;&#x430; &#x43C;&#x435;&#x434;&#x430;&#x43B;&#x456; &#x430;&#x431;&#x43E; &#x43C;&#x43E;&#x43D;&#x435;&#x442;&#x456;</trd>
  <trdgrp lng="zh">
    <trd>&#x9298; <pr>m&#xED;ng</pr></trd>,
    <trd>&#x9298;&#x523B; <pr>m&#xED;ngk&#xE8;</pr></trd>
  </trdgrp>
</drv>
</art>
