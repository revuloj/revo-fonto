<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lian.xml,v 1.20 2019/12/13 19:10:28 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lian</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="lian.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lian.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Grimpa <ref tip="super" cel="veget.0ajxo">vegeta&jcirc;o</ref>, kiu
      volvi&gcirc;as &ccirc;irka&ubreve;
      arbo a&ubreve; alia apoga&jcirc;o:
      <ekz>
        la dornoplenaj <tld/>oj prezentas netrapenetreblan densejon
        inter la arboj, kie
        la akvoserpento ku&scirc;as en la malseka herbo
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekkreskis anka&ubreve; <tld/>oj kaj aerradikoj de supre malsupren kaj
        interteksi&gcirc;is kun
        la densa&jcirc;o al netrairebla arbusta&jcirc;o
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Perelin', la Noktarbaro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_ja;&c_n;&c_a;</trd>
  <trd lng="cs">li&aacute;na</trd>
  <trd lng="de">Liane</trd>
  <trd lng="el">&kappa;&lambda;&eta;&my;&alfa;&tau;&jota_ton;&delta;&alfa;</trd>
  <trdgrp lng="en">
    <trd>liana</trd>,
    <trd>tropical climbing vine</trd>
  </trdgrp>
  <trd lng="es">liana</trd>
  <trd lng="fr">liane</trd>
  <trd lng="hu">li&aacute;n</trd>
  <trd lng="nl">liaan</trd>
  <trd lng="pl">liana</trd>
  <trdgrp lng="pt">
    <trd>cip&oacute;</trd>,
    <trd>icip&oacute;</trd>,    
    <trd>corda <klr>(STP)</klr></trd>,
    <trd>liana</trd>    
  </trdgrp>
  <trd lng="ro">lian&abreve;</trd>
  <trd lng="ru">&c_l;&c_i;&c_a;&c_n;&c_a;</trd>
  <trd lng="sk">liana</trd>
  <trd lng="sv">lian</trd>
</drv>
</art>
