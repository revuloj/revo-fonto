<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tar1.xml,v 1.16 2024/01/11 23:27:08 revo Exp $">
<kap>
  <rad>tar</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tar1.0o">
  <kap><tld/>o</kap>
  <snc mrk="tar1.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="fak">KUI</uzo>
    <dif>
      Plurjara vegeta&jcirc;o kun grandaj folioj, kultivata en tropikaj landoj pro man&gcirc;eblaj
      rizomo kaj folioj; <ref tip="lst" lst="voko:botanikaj_specioj" cel="speci.0o.BIO">specio</ref>
      el la genro <ref tip="super" cel="kolokazi.0o.BOT">kolokazio</ref>
      (<trd lng="la">Colocasia esculenta</trd>):
      <ekz>
        en la monto &ccirc;efa man&gcirc;a&jcirc;o estis radiktuberoj kiel batato,
        <tld/>o, manioko
        <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>la perdita gusto de
        Pinatubo, p. 24a</lok></fnt>.
      </ekz>
    </dif>
    <bld
    lok="&WCU;/thumb/6/66/Colocasia_esculenta_P1190432.jpg/640px-Colocasia_esculenta_P1190432.jpg"
    prm="CC-BY-SA-3.0"><tld/>otuberoj
        <fnt>
          <aut>David Monniaux, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Colocasia_esculenta_P1190432.jpg">&WCW;/File:Colocasia_esculenta_P1190432.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <ref tip="hom" cel="tar.0o">taro (ujomaso)</ref>
  <trd lng="be">&c_t;&c_a;&c_r;&c_a; <klr tip="amb">(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trd lng="ca">taro</trd>
  <trdgrp lng="cs">
    <trd>t&aacute;ra</trd>,
    <trd>v&aacute;ha obalu</trd>
  </trdgrp>
  <trd lng="de">Taro</trd>
  <trd lng="en">taro</trd>
  <trdgrp lng="fr">
    <trd>arouille violette</trd>,
    <trd>chou de chine</trd>,
    <trd>dachine</trd>,
    <trd>taro</trd>
  </trdgrp>
  <trd lng="ja">&#x98A8;&#x888B;  <pr>&#12405;&#12358;&#x305F;&#12356;</pr></trd>
  <trdgrp lng="pl">
    <trd>taro</trd>,
    <trd>kolokazja jadalna</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>taioba</trd>,
    <trd>inhame</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>tara</trd>,
    <trd>v&aacute;ha obalu</trd>
  </trdgrp>
  <trd lng="sv">taro</trd>
</drv>
</art>

