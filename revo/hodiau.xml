<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>hodia&ubreve;</rad>
</kap>
<drv mrk="hodiau.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Adverbo signifanta &leftquot;en la nuna tago&rightquot;:
      <ekz>
        <tld/> vespere;
      </ekz>
      <ekz>
        <tld/> estas festo;
      </ekz>
      <ekz>
        <tld/> por mono, morga&ubreve; senpage<fnt>Z</fnt>;
      </ekz>
      <ekz><tld/> forto, morga&ubreve; morto<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        en la <tld/>a tago.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
   <trd>&c_s;&c_jo;&c_n;&c_mol;&c_n;&c_ja;</trd>,
   <trd>&c_s;&c_ja;&c_g;&c_o;&c_d;&c_n;&c_ja;</trd>,
   <trd>&c_s;&c_ja;&c_g;&c_o;&c_n;&c_mol;&c_n;&c_ja;</trd>,
  </trdgrp>                 
  <trd lng="br">hiziv</trd>
  <trd lng="cs">dnes</trd>
  <trd lng="de">heute</trd>
  <trd lng="el">&sigma;&eta_ton;&my;&epsilon;&rho;&alfa;</trd>
  <trd lng="es">hoy</trd>
  <trd lng="en">today</trd>
  <trd lng="fr">aujourd'hui</trd>
  <trd lng="he">&he;&yod;&vav;&fmem;</trd>
  <trd lng="hu">ma</trd>
  <trd lng="it">oggi</trd>
  <trd lng="nl">vandaag</trd>
  <trd lng="pl">dzi&sacute;</trd>
  <trd lng="pt">hoje</trd>
  <trdgrp lng="ro">
   <trd>ast&#x103;zi</trd>,
   <trd>azi</trd>,
  </trdgrp>
  <trd lng="ru">&c_s;&c_je;&c_g;&c_o;&c_d;&c_n;&c_ja;</trd>
  <trd lng="sv">i <ind>dag</ind></trd>
  <trd lng="tr">bug&uuml;n</trd>
</drv>
