<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>hodia&ubreve;</rad></kap>
<drv mrk="hodiau.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Adverbo signifanta &leftquot;en la nuna tago&rightquot;:
      <ekz>
        <tld/> vespere;
      </ekz>
      <ekz>
        <tld/> estas festo;
      </ekz>
      <ekz>
        <tld/> por mono, morga&ubreve; senpage<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/> forto, morga&ubreve; morto
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
      </ekz>
      <ekz>
        en la <tld/>a tago.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">dnes</trd>
  <trd lng="de">heute</trd>
  <trd lng="es">hoy</trd>
  <trd lng="en">today</trd>
  <trd lng="fr">aujourd'hui</trd>
  <trd lng="nl">vandaag</trd>
  <trd lng="pl">dzi&sacute;</trd>
  <trd lng="tr">bug&uuml;n</trd>
</drv>
</art>
