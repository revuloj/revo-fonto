<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vernier.xml,v 1.11 2022/12/28 07:25:22 revo Exp $">
<kap>
  <rad>vernier</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="vernier.0o">
  <kap><tld/>o</kap>
  <snc mrk="vernier.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Preciza <ref tip="super" cel="mezur.0ilo.TEK">mezurilo</ref> de 
      distanco kun subskala parto glitigebla sur &ccirc;efskala parto:
      <ekz>
        la de&jcirc;oranto kapjesis kaj komencis operacii kun butonoj kaj 
        <tld/>oj de la memorma&scirc;ino
        <fnt>
          <aut>Ivan Efremov, trad. Jurij Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Efremov/andromedo.html">La nebulozo de
Andromedo</url></vrk>,
          <lok>&ccirc;apitro 10a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_e;&c_r;&c_n;&c_mol;&c_je;&c_r;</trd>
  <trdgrp lng="ca">
    <trd>n&ograve;nius</trd>,
    <trd>vernier</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nonius</trd>,
    <trd>vernier</trd>
  </trdgrp>
  <trd lng="de">Nonius</trd>
  <trdgrp lng="en">
    <trd>vernier scale</trd>,
    <trd>vernier caliper</trd>
  </trdgrp>
  <trd lng="fr">vernier</trd>
  <trd lng="hu">n&oacute;niusz</trd>
  <trd lng="nl">vernier</trd>
  <trdgrp lng="pl">
   <trd>wernier</trd>,
   <trd>noniusz</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>n&ocirc;nio</trd>,
   <trd>vernier</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nonius</trd>,
    <trd>vernier</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_je;&c_r;&c_n;&c_mol;&c_jeu;&c_r;</trd>,
    <trd>&c_n;&c_o;&c_n;&c_ib;&c_u;&c_s;</trd>
  </trdgrp>
</drv>
</art>

