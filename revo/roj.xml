<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: roj.xml,v 1.15 2025/04/05 10:02:33 revo Exp $">
<kap>
   <rad>roj</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="roj.0o">
 <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Natura akvofluo, pli malgranda ol <ref tip="vid" cel="river.0eto">rivereto</ref>, ordinare ne
      surnavigebla:
      <ekz>
        la klara akvo de la <tld/>oj servas al mi kiel
        spegulo
        <fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 14a, p.
        101a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>rivereto</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_r;&c_u;&c_ch;&c_a;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_w;&c_ch;&c_u;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_p;&c_o;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_r;&c_u;&c_ch;&c_je;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>potok</trd>,
    <trd>ron <klr>(vody)</klr></trd>
  </trdgrp>
  <trd lng="de">Bach<klr> (Gew&auml;sser)</klr></trd>
  <trd lng="en">stream</trd>
  <trd lng="es">arroyo</trd>
  <trd lng="fr">ruisseau</trd>
  <trdgrp lng="hu">
    <trd>patak</trd>,
    <trd>csermely</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>strumie&nacute;</trd>,
    <trd>potok</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>arroio</trd>,
    <trd>regato</trd>
  </trdgrp>
  <trd lng="ro">p&acirc;r&acirc;u</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_u;&c_ch;&c_je;&c_j;</trd>,
    <trd>&c_k;&c_l;&c_ju;&c_ch;<klr tip="ind"> (&c_r;&c_u;&c_ch;&c_je;&c_j;)</klr></trd>
  </trdgrp>
  <trd lng="sk">potok</trd>
  <trdgrp lng="uk">
    <trd>&#1087;&#1086;&c_t;&c_ib;&#1082;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_u;&#1084;&#1086;&#1082;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#23567;&#28330; <pr>xi&#462;ox&#299;</pr></trd>,
    <trd>&#28330;&#27969; <pr>x&#299;li&#250;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="roj.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Fluadi simile al <tld/>o:
      <ekz>
        &scirc;vito <tld/>is sur &scirc;ia korpo
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 21a, p. 206a</lok></fnt>;
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_ja;&c_ch;&c_y; &c_ja;&c_k; &c_r;&c_u;&c_ch;&c_a;&c_j;</trd>
  <trdgrp lng="en">
     <trd>stream</trd>
     <trd>trickle</trd>
  </trdgrp>
  <trd lng="fr">ruisseler</trd>
  <trdgrp lng="hu">
    <trd>patakzik</trd>,
    <trd>csurog</trd>
  </trdgrp>
  <trd lng="pl">sp&lstroke;ywa&cacute;</trd>
  <trd lng="ro">curge</trd>
  <trd lng="ru">&c_t;&c_je;&c_ch;&c_mol; <ind>&c_r;&c_u;&c_ch;&c_mol;&c_jo;&c_m;</ind></trd>
  <trd lng="zh">&#27969;&#28108; <pr>li&#250;t&#462;ng</pr></trd>
</drv>
</art>

