<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>anilin</rad>/o
</kap>
<drv mrk="anilin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      C<sub>6</sub>H<sub>7</sub>N. Senkolora flua&jcirc;o, tirata el terkarba
      gudro, kaj uzata en fabrikado de koloriloj.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_ib;&c_l;&c_ib;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>aminobenzen</trd>,
    <trd>anilin</trd>
  </trdgrp>
  <trd lng="de">Anilin</trd>
  <trd lng="en">aniline <klr>(phenylamine)</klr></trd>
  <trd lng="es">anilina</trd>
  <trd lng="fr">aniline</trd>
  <trd lng="nl">aniline</trd>
  <trd lng="ru">&c_a;&c_n;&c_i;&c_l;&c_i;&c_n;</trd>
  <trd lng="sk">anil&iacute;n</trd>
</drv>
</art>
