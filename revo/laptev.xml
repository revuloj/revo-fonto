<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Laptev</rad>/o <fnt><bib>BoER</bib></fnt>
    </kap>
    <drv mrk="laptev.L0aMaro">
      <kap><tld/>a Maro</kap>
      <snc>
	<uzo tip="fak">GEOG</uzo>
	<dif>
	  Maro &ccirc;e la <ref tip="malprt" cel="siberi.S0o">Siberia</ref> bordo
	  de la <ref tip="malprt" cel="arkt.A0aOceano">Arkta Oceano</ref>,
	  inter la <ref cel="kara.0aMaro">Karaa Maro</ref> kaj la
	  <ref cel="siberi.Orient0aMaro">Orientsiberia Maro</ref>.
	</dif>
	<rim>
	  Nomita oma&gcirc;e al la kuzoj Laptev (Demetrio kaj &Hcirc;aritono),
	  kiuj esploris la regionon en 1746&ndash;1742.
	</rim>
      </snc>
      <trd lng="be">&c_m;&c_o;&c_r;&c_a; &c_L;&c_a;&c_p;&c_c;&c_je;&c_v;&c_y;&c_h;</trd>
      <trd lng="en">Laptev Sea</trd>
      <trd lng="hu">Laptyev-tenger</trd>
      <trd lng="pt">mar de <ind>Laptev</ind></trd>
      <trd lng="ru">&c_m;&c_o;&c_r;&c_je; &c_L;&c_a;&c_p;&c_t;&c_je;&c_v;&c_y;&c_h;</trd>
    </drv>

  </art>

