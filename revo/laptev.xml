<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Laptev</rad>/o <fnt><bib>BoER</bib></fnt>
</kap>
<drv mrk="laptev.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">PERS</uzo>
    <dif>
      Ruse <trd lng="ru">&c_L;&c_a;&c_p;&c_t;&c_je;&c_v;</trd>,
      familia nomo de du kuzoj (Demetrio kaj &Hcirc;aritono),
      kiuj esploris en 1746&ndash;1742 la regionon &ccirc;e la bordo
      de la oma&gcirc;e al ili nun nomata <ref cel="mar.LaptevaM0o"><tld/>a
      Maro</ref>.
    </dif>
  </snc>
  <trd lng="fr">Laptev</trd>
  <trd lng="hu">Laptyev</trd>
  <trd lng="nl">Laptev</trd>
  <trd lng="pl">&Lstroke;aptiew</trd>
</drv>
</art>
