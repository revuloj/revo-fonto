<vortaro>
<art mrk="$Id: mohajr.xml,v 1.6 2022/09/19 19:31:25 revo Exp $">
<kap>
  <rad>mohajr</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="mohajr.0o">
  <kap><tld/>o</kap>
  <snc mrk="mohajr.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Delikata, tre varm- kaj malvarm-imuna
      <ref tip="super" cel="sxtof.0o.TEKS">&scirc;tofo</ref>
      el lano de anguraj kaproj:
      <ekz>
        vilaj <tld/>oj
        <fnt>
         <aut/>,
         <vrk><url ref="http://epo.wikitrans.net/Zika_Ascher?eng=Zika+Ascher">
         Zika Ascher</url></vrk>,
         <lok>WikiTrans, 2013-01-09</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_h;&c_je;&#769;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>moh&eacute;r</trd>,
    <trd>moh&eacute;rov&aacute; l&aacute;tka</trd>
  </trdgrp>
  <trd lng="en">mohair</trd>
  <trd lng="fr">mohair</trd>
  <trd lng="pl">moher</trd>     
  <trd lng="ro">mohair</trd>
  <trd lng="sk">moh&eacute;rov&aacute; l&aacute;tka</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x43E;&c_h;&c_je;&c_r;</trd>,
    <trd>&c_v;&#x43E;&c_v;&#x43D;&c_a; &c_a;&#x43D;&c_gu;&#x43E;&c_r;&c_s;&#x44C;&#x43A;&#x43E;&c_ji; &#x43A;&#x43E;&c_z;&c_i;</trd>,
    <trd>&c_t;&#x43A;&c_a;&#x43D;&c_i;&#x43D;&c_a; &c_z; &c_v;&#x43E;&c_v;&#x43D;&c_i; &c_a;&#x43D;&c_gu;&#x43E;&c_r;&c_s;&#x44C;&#x43A;&#x43E;&c_ji;
    &#x43A;&#x43E;&c_z;&c_i;</trd>
  </trdgrp>
</drv>
</art>

