<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kaskad</rad>/o <fnt>Z</fnt>
</kap>
<drv mrk="kaskad.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Plur&scirc;tupa akvofalo.
    </dif>
    <trd lng="nl">waterval <klr tip="ind">(kleine)</klr></trd>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Rapida, neretenebla fluo de io:
      <ekz>senfina <tld/>o da demandoj.</ekz>
    </dif>
  </snc>
  <snc num="3">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Grupo de sinsekve kunligitaj samtipaj aparatoj a&ubreve; konstrua&jcirc;oj:
      <ekz><tld/>a sistemo,</ekz>
      <ekz><tld/>a konekto,</ekz>
      <ekz><tld/>a amplifilo.</ekz>
    </dif>
  </snc>
  <trd lng="en">cascade</trd>
  <trd lng="ru">&c_k;&c_a;&c_s;&c_k;&c_a;&c_d;</trd>
</drv>
</art>
