<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kaskad</rad>/o <fnt>Z</fnt>
</kap>
<drv mrk="kaskad.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Plur&scirc;tupa akvofalo.
    </dif>
    <trdgrp lng="de">
      <trd>Kaskade</trd>,
      <trd>Stufenwasserfall</trd>
    </trdgrp>
    </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Rapida, neretenebla fluo de io:
      <ekz>senfina <tld/>o da demandoj.</ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Kaskade</trd>,
      <trd>Schwall</trd>
    </trdgrp>
  </snc>
  <snc num="3">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Grupo de sinsekve kunligitaj samtipaj aparatoj a&ubreve; konstrua&jcirc;oj:
      <ekz><tld/>a sistemo,</ekz>
      <ekz><tld/>a konekto,</ekz>
      <ekz><tld/>a amplifilo.</ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Kaskade</trd>,
      <trd>Batterie</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_s;&c_k;&c_a;&c_d;</trd>
  <trd lng="en">cascade</trd>
  <trd lng="nl">cascade</trd>
  <trd lng="ru">&c_k;&c_a;&c_s;&c_k;&c_a;&c_d;</trd>
</drv>
</art>
