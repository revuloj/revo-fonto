<art mrk="$Id: repetitor.xml,v 1.3 2023/11/24 15:26:11 afido Exp $">
<kap>
    <rad>repetitor</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="repetitor.0o">
  <kap><tld/>o</kap>
  <snc mrk="repetitor.0o.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      Privata <ref tip="super" cel="instru.0isto.PED">instruisto</ref>, kiu lecionas ekster
      la lerneja sistemo:
      <ekz>
        &scirc;iaj <tld/>oj helpis ricevi konfirmojn pri gimnaziaj klasoj
        <fnt>
          <aut>V. Marti&scaron;auskas, V. Banaitis</aut>,
          <vrk><url ref="http://barelo.blogspot.fr/2012/05/bonkora-doktoro-banaitis-tutejsa.html">
          Bonkora Doktoro Banaitis...</url></vrk>,
          <lok>Barelo, 2012-05-11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="eduk.0isto.homo">edukisto</ref>,
    <ref cel="guvern.0isto.PED">guvernisto</ref>
  </refgrp>
  <trd lng="en">tutor</trd>
  <trd lng="fr">r&eacute;p&eacute;titeur</trd>
</drv>
</art>

