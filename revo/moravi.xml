<vortaro>
  <art mrk="$Id: moravi.xml,v 1.1 2023/07/24 16:30:24 afido Exp $">
    <kap>
      <rad>Moravi</rad>/o
    </kap>

<drv mrk="moravi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Orienta parto de <ref tip="malprt" cel="cxehx.CX0ujo">&Ccirc;e&hcirc;ujo</ref>:
      <ekz>
        li venkis la a&ubreve;strojn <klr>[&#8230;]</klr> kaj rusojn &ccirc;e
        <nom>A&ubreve;sterlic</nom> en <tld/>o
        <fnt>
          <bib>SrT</bib>
          <lok>Libro I.</lok>
        </fnt>;
      </ekz>
      <ekz>
        post multaj skribaj intervenoj el Bohemio kaj <tld/>o <klr>[&#8230;]</klr> estis
        promesite, ke la 5an de junio <nom>Hus</nom> rajtos paroli
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007031.html"
              >Katolikismo: Johano Hus &ndash; &ccirc;u ne plu herezulo?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">M&auml;hren</trd>
    <trd lng="en">Moravia</trd>
    <trd lng="fr">Moravie</trd>
  </snc>
</drv>

  </art>

