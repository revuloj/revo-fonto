<vortaro>
<art mrk="$Id$">
<kap><rad>higumen</rad>/o</kap>
<drv mrk="higumen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KRI</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="igumen.0o">Igumeno</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_g;&c_u;&c_m;&c_je;&c_n;</trd>
  <trd lng="hu">ap&aacute;t <klr tip="amb">(pravoszl&aacute;v)</klr></trd>
  <trd lng="pt">superior de um mosteiro</trd> 
  <trd lng="ru">&c_i;&c_g;&c_u;&c_m;&c_je;&c_n;</trd>
</drv>
<drv mrk="higumen.0ino">
