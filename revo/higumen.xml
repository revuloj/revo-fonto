  <uzo tip="fak">KRI</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="igumen.0ino">Igumenino</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_g;&c_u;&c_m;&c_je;&c_n;&c_mol;&c_n;&c_ja;</trd>
  <trd lng="hu">ap&aacute;tn&odblac; <klr tip="amb">(pravoszl&aacute;v)</klr></trd>    
