<drv mrk="pak.mal0i">
  <kap>el<tld/>i, <var><kap>mal<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pak.mal0i.KOMUNE">
    <dif>
      Elmeti el <tld/>o:
      <ekz>
        la kestojn kaj &scirc;nuregojn, &ccirc;ion ili devis el<tld/>i
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        en kelo de vinkomercisto <klr>[la botelo]</klr> estis elpakita
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>;
      </ekz>
      <ekz>
        el<tld/>i kaj en<tld/>i
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> mal<tld/>is la lastan el la kestoj, en kiuj
        alvenis lia poseda&jcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2009/009682.php">La
           Pendantaj &Gcirc;ardenoj estis son&gcirc;o</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        alvenis koresponda&jcirc;o <klr>[&#x2026;]</klr>, mi tuj mal<tld/>is &gcirc;in
        <fnt>
          <bib>Monato</bib>
          <aut>Paulo Silas</aut>
          <vrk><url ref="&Monato;/2015/011507.php">La
           lastaj tagoj de Lillo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e IKEA vi a&ccirc;etas kelkajn <tld/>a&jcirc;ojn, devas mem 
        transporti ilin hejmen, el<tld/>i el multaj plastaj folioj
        kaj &scirc;a&ubreve;mmaterialo kaj mem munti &scirc;rankon
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2010/010006.php">A&ubreve;tomat-homo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">KOMP</uzo>
        la el<tld/>ita programo  bezonas preska&ubreve; 6 megabitokojn
        <fnt>
            <aut>Pa&ubreve;l Peeraerts</aut>
            <bib>Monato</bib>
            <vrk><url ref="&Monato;/1995/A950501.php">Ni levu la &ccirc;apelon!</url></vrk>
            <lok>1995/05, p. 16</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_p;&c_a;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_r;&c_a;&c_z;&c_o;&c_p;&c_a;&c_k;&c_o;&c_v;&c_a;&c_m;</trd>
