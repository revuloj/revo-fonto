<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc><rad>pak</rad>/i <fnt><bib>PV</bib></fnt>
    </kap>

    <drv mrk="pak.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="pak.0i.enujigi">
        <dif>
          Kunaran&gcirc;i malvaste iajn objektojn, &ccirc;u ilin ligante per
          &scirc;nuro, &ccirc;u envolvante en paperon a&ubreve; &scirc;tofon, &ccirc;u en&scirc;ovante en
          sakon a&ubreve; alian portilon, &ccirc;u ordigante en kesto:
          <ekz>
            li <tld/>is sian hava&jcirc;on<fnt>Z</fnt>;
          </ekz><ekz>
            la komerca&jcirc;oj devas esti <tld/>ataj<fnt>Z</fnt>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            li estis <tld/>ita kun ok aliaj personoj en la interno
            de po&scirc;ta kale&scirc;o<fnt>Z</fnt>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            ni estis prem<tld/>itaj en la &ccirc;ambro.
          </ekz>
        </dif>
      </snc>
      <snc mrk="pak.0i.plenigi">
        <dif>
          <ref tip="vid" cel="plen.0igi">Plenigi</ref> per a&jcirc;oj kaj
          pretigi tranportilon:
          <ekz>
            mi senprokraste <tld/>is miajn kofrojn, adia&ubreve;is Brigidon kaj
            reveturis
            <fnt>
              <bib>FK</bib>,
              <lok>Rakonto pri Spiritoj</lok>
            </fnt>;
          </ekz><ekz>
            Knut iris en sian lo&gcirc;ejon kaj <tld/>is sian tornistron
            <fnt><bib>Fab3</bib>, <lok>Sub la saliko</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_p;&c_a;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_u;&c_p;&c_a;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_u;&c_k;&c_l;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_c;&c_ju;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
      </trdgrp>
      <trd lng="bg">&c_o;&c_p;&c_a;&c_k;&c_o;&c_v;&c_a;&c_m;</trd>
      <trd lng="cs">balit</trd>
      <trd lng="de">packen</trd>
      <trd lng="en">pack</trd>
      <trd lng="es">empaquetar</trd>
      <trdgrp lng="fr">
        <trd>emballer</trd>,
        <trd>empaqueter</trd>
      </trdgrp>
      <trd lng="hu">csomagol</trd>
      <trdgrp lng="nl">
        <trd>pakken</trd>,
        <trd>inpakken</trd>
      </trdgrp>
      <trdgrp lng="pt">
        <trd>embrulhar</trd>,
        <trd>empacotar</trd>,
        <trd>enfardar</trd>,
        <trd>emalar</trd>,
        <trd>empalhar</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_p;&c_a;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_u;&c_p;&c_a;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
    </drv>

    <drv mrk="pak.0o">
      <kap><tld/>o</kap>
      <snc mrk="pak.0o.KOMUNE">
        <dif>
          Aro da <tld/>itaj objektoj:
          <ekz>
            <ind>pli kostas la sako ol la tuta <tld/>o</ind>
            <trd lng="hu">t&ouml;bbe ker&uuml;l a h&uacute;s, mint a leves</trd>
            (<ref tip="vid" cel="kandel.0o">kandelo</ref>)
            <fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            mi dissigelis la <ind><tld/>eto</ind>n, kiun ili kun si tre
            garde portis
            <fnt>Z</fnt>.
            <trdgrp lng="hu">
              <trd>kis <ind>csomag</ind></trd>,
              <trd>csomagocska</trd>
            </trdgrp>
            <trd lng="ru">&c_p;&c_a;&c_k;&c_je;&c_t;</trd>
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_p;&c_a;&c_k;&c_je;&c_t;</trd>,
        <trd>&c_u;&c_p;&c_a;&c_k;&c_o;&c_w;&c_k;&c_a;</trd>,
        <trd>&c_c;&c_ju;&c_k;</trd>
      </trdgrp>
      <trd lng="bg">&c_p;&c_a;&c_k;&c_je;&c_t;</trd>
      <trd lng="de">Paket</trd>
      <trd lng="en">package</trd>
      <trd lng="es">paquete</trd>
      <trd lng="fr">paquet</trd>
      <trd lng="hu">csomag</trd>
      <trd lng="nl">pak</trd>
      <trdgrp lng="ru">
        <trd>&c_p;&c_a;&c_k;&c_je;&c_t;</trd>,
        <trd>&c_u;&c_p;&c_a;&c_k;&c_o;&c_v;&c_k;&c_a;</trd>.
        <trd>&c_t;&c_ju;&c_k;</trd>
      </trdgrp>
    </drv>

    <drv mrk="pak.0ajxo">
      <kap><tld/>a&jcirc;o</kap>
      <snc mrk="pak.0ajxo.unupeco">
        <ref tip="dif" cel="pak.0o"><tld/>o</ref>
        <ekz>
          Jim elprenis <tld/>a&jcirc;on el sia surtuta po&scirc;o kaj &jcirc;etis
          &gcirc;in sur la tablon
          <fnt>
            <aut>O. Henry, trad. D. J. Harlow</aut>,
            <vrk>La donaco de la magoj</vrk>,
            <bib>LOdE</bib>, <lok>2001, numero 12a</lok>
          </fnt>;
        </ekz><ekz>
          legantoj de Monato, kiuj volas helpi forigi malsaton inter
          la infanoj de la mondo, povas <klr>...</klr> a&ccirc;eti tiajn
          <tld/>a&jcirc;ojn kun 30 kompletaj man&gcirc;oj <klr>...</klr> kontra&ubreve; po
          31 e&ubreve;roj
          <fnt>
            <aut>P. Peeraerts</aut>,
            <vrk>Nova ha&ubreve;to por la a&ubreve;to-&ccirc;efo</vrk>,
            <bib>Monato</bib>,
            <lok>numero 2002-11, p.14a</lok>
          </fnt>.
        </ekz>
        <trdgrp lng="be">
          <trd>&c_p;&c_a;&c_k;&c_je;&c_t;</trd>,
          <trd>&c_p;&c_a;&c_k;&c_u;&c_n;&c_a;&c_k;</trd>
        </trdgrp>
        <trd lng="bg">&c_p;&c_a;&c_k;&c_je;&c_t;</trd>
        <trd lng="de">P&auml;ckchen</trd>
        <trd lng="en">package</trd>
        <trd lng="fr">paquet</trd>
        <trdgrp lng="nl">
          <trd>pak</trd>,
          <trd>verpakking</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_p;&c_a;&c_k;&c_je;&c_t;</trd>,
          <trd>&c_u;&c_p;&c_a;&c_k;&c_o;&c_v;&c_k;&c_a;</trd>.
          <trd>&c_t;&c_ju;&c_k;</trd>
        </trdgrp>
      </snc>
      <snc mrk="pak.0ajxo.bagagxo">
        <ref tip="dif" cel="bagagx.0o">baga&gcirc;o</ref>
        <ekz>
          kiam mi veturas, mi neniam prenas kun mi multon da
          <tld/>a&jcirc;o<fnt>Z</fnt>;
        </ekz><ekz>
          <ind>forportu vian <tld/>a&jcirc;on kaj hava&jcirc;on</ind>
          <trd lng="fr">prenez vos cliques et vos claques</trd>
          <klr>(definitive foriru)</klr>
          <fnt><bib>PrV</bib></fnt>.
        </ekz>
        <trd lng="be">&c_b;&c_a;&c_g;&c_a;&c_zh;</trd>
        <trd lng="bg">&c_b;&c_a;&c_g;&c_a;&c_zh;</trd>
        <trd lng="de">Gep&auml;ck</trd>
        <trd lng="en">luggage</trd>
        <trd lng="fr">bagage</trd>
        <trd lng="hu">poggy&aacute;sz</trd>
        <trd lng="nl">bagage</trd>
        <trd lng="ru">&c_b;&c_a;&c_g;&c_a;&c_zh;</trd>
      </snc>
    </drv>

    <drv mrk="pak.0ajxujo">
      <kap><tld/>a&jcirc;ujo</kap>
      <uzo tip="fak">AUT</uzo>
      <snc>
        <dif>
          Fermebla (kaj kutime anka&ubreve; &scirc;losebla) parto de veturilo
          (precipe a&ubreve;tomobilo), destinita por
          transporti <tld/>a&jcirc;ojn (valizojn, sakojn ktp) a&ubreve; aliajn
          objektojn kiujn oni malvolas kunpreni en la kabinon:
          <ekz>
            Dirinte tre lakone:
            <ctl>La ha&ubreve;to ta&ubreve;gas por bona mansako</ctl>,
            <klr>la &scirc;oforo</klr> senhezite &jcirc;etis la serpenton en la
            <tld/>a&jcirc;ujon de sia a&ubreve;to kaj forveturis
            <fnt>
              <aut>Werner SCHAD</aut>:
              <vrk>Serpentoj</vrk>. <bib>Monato</bib>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="bg">&c_b;&c_a;&c_g;&c_a;&c_zh;&c_n;&c_i;&c_k;&c_a;</trd>
      <trd lng="de">Kofferraum</trd>
      <trdgrp lng="en">
          <trd>boot</trd>
          <trd>trunk</trd>
      </trdgrp>
      <trd lng="fr">tronc</trd>
      <trd lng="nl">kofferruimte</trd>
      <trd lng="pl">baga&zdot;nik</trd>
      <trd lng="ru">&c_b;&c_a;&c_g;&c_a;&c_zh;&c_n;&c_i;&c_k;</trd>
      <trd lng="tr">bagaj yeri</trd>
    </drv>

    <drv mrk="pak.0umo">
      <kap><tld/>umo<fnt>PIV1</fnt></kap>
      <snc>
        <dif>
          Entenanta&jcirc;o, kovra&jcirc;o uzata por <tld/>i:
          <ekz>
            kvankam <tld/>umo estas vesto de varo kaj povas beligi
            &gcirc;in, tamen por simpla varo troa <tld/>umo ne nur pliigas
            la koston, sed anka&ubreve; ka&ubreve;zas grandan mal&scirc;paron de
            naturaj ri&ccirc;fontoj
            <fnt>
              <aut>Mu Binghua</aut>,
              <vrk>Arbarego por pakumi &ccirc;emizojn</vrk>,
              <bib>Monato</bib>,
              <lok>numero 2001-09, p. 10a</lok>
            </fnt>.
          </ekz>
        </dif>
        <ref tip="vid" cel="tar.0o">taro</ref>
      </snc>
      <trd lng="bg">&c_o;&c_p;&c_a;&c_k;&c_o;&c_v;&c_a;&c_n;&c_je;</trd>
      <trd lng="de">Verpackung</trd>
      <trdgrp lng="en">
          <trd>packing</trd>
          <trd>wrapping</trd>
      </trdgrp>
      <trd lng="fr">emballage</trd>
      <trdgrp lng="hu">
        <trd>csomagol&aacute;s<klr tip="ind">(g&ouml;ngy&ouml;leg)</klr></trd>,
        <trd>csomagol&oacute;anyag</trd>,
        <trd>g&ouml;ngy&ouml;leg</trd>
      </trdgrp>
      <trd lng="nl">verpakking</trd>
      <trdgrp lng="ru">
        <trd>&c_u;&c_p;&c_a;&c_k;&c_o;&c_v;&c_k;&c_a;</trd>,
        <trd>&c_t;&c_a;&c_r;&c_a;</trd>
      </trdgrp>
    </drv>

    <drv mrk="pak.mal0i">
      <kap>el<tld/>i<fnt>Z</fnt>, <var><kap>mal<tld/>i</kap></var></kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="pak.mal0i.KOMUNE">
        <dif>
          Elmeti el <tld/>o.
        </dif>
      </snc>
      <trd lng="be">&c_r;&c_a;&c_s;&c_p;&c_a;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
      <trd lng="bg">&c_r;&c_a;&c_z;&c_o;&c_p;&c_a;&c_k;&c_o;&c_v;&c_a;&c_m;</trd>
      <trd lng="de">auspacken</trd>
      <trd lng="en">unpack</trd>
      <trd lng="fr">d&eacute;baller</trd>
      <trd lng="hu">kicsomagol</trd>
      <trd lng="nl">uitpakken</trd>
      <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_a;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </drv>

    <drv mrk="pak.en0i">
      <kap>en<tld/>i<fnt>Z</fnt></kap>
      <snc mrk="pak.en0i.KOMUNE">
        <dif>
          Meti en <tld/>on.
        </dif>
      </snc>
      <trd lng="be">&c_u;&c_p;&c_a;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
      <trd lng="de">einpacken</trd>
      <trd lng="fr">emballer</trd>
      <trd lng="hu">becsomagol</trd>
      <trd lng="nl">inpakken</trd>
      <trd lng="ru">&c_u;&c_p;&c_a;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </drv>
  </art>
