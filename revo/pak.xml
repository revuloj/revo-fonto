<drv mrk="pak.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pak.0i.enujigi">
    <dif>
      Kunaran&gcirc;i malvaste iajn objektojn, ordinare preparante stokadon a&ubreve;
      transportadon,
      &ccirc;u ilin ligante, &ccirc;u envolvante, &ccirc;u en&scirc;ovante en
      sakon a&ubreve; alian portilon, &ccirc;u ordigante en kesto:
      <ekz>
         frue en la sekvanta mateno li <tld/>is sian malgrandan 
         hava&jcirc;on<fnt><bib>Fab1</bib>,
         <lok>Vojkamarado</lok></fnt>;
      </ekz>
      <ekz>
        la patrino <klr>[&#x2026;]</klr> pakis tion a&ubreve; alian, kun 
        kio la patro estis ironta hodia&ubreve; vespere en la urbon
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is &ccirc;iujn miajn pupojn en la lignan keston, 
        alligis &gcirc;in al mia dorso <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Fab4</bib>
          <lok>Pupludisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili kolektis nur la spikojn, kiujn ili <tld/>is en sakojn,
        la pajlo restis sur la kampo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        paketoj, <klr>[&#x2026;]</klr> tiel firme kunmetitaj kaj tiel
        lerte <tld/>itaj unuj sur la aliaj, ke ili &ccirc;e &ccirc;iu movo 
        de la &scirc;ipo ne ruli&gcirc;as tien kaj reen
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        multaj kukoj estas unuope <tld/>itaj per plasto, tiuj kukoj
        siavice estas anka&ubreve; grupe volvitaj en plasto, kaj finfine tio
        estas <tld/>ita en kartona skatolo
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van de Velde kaj Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2014/011347.php">Konsumantoj
           agiti&gcirc;as kaj konscii&gcirc;as</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        presigi &ccirc;iujn tekstojn per tute etaj literoj kaj 
        tiel <tld/>i multe pli da enhavo sur &ccirc;iun pa&gcirc;on
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2003/008130.html">&Scirc;topi&gcirc;o</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li estis <tld/>ita kun ok aliaj personoj en la interno
        de po&scirc;ta kale&scirc;o<fnt><bib>Fab1</bib>, <lok>Galo&scirc;oj de 
        feli&ccirc;o</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        oni povis ricevi kapturnon de tio, kiel la homoj en 
        tiu komplikita amaso da stratoj estis <tld/>itaj
        unuj super la aliaj
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="pak.0i.plenigi">
    <dif>
      <ref tip="vid" cel="plen.0igi">Plenigi</ref> per a&jcirc;oj kaj
      pretigi transportilon:
      <ekz>
        mi senprokraste <tld/>is miajn kofrojn, adia&ubreve;is Brigidon kaj
        reveturis
        <fnt>
          <bib>FK</bib>,
          <lok>Rakonto pri Spiritoj</lok>
        </fnt>;
      </ekz><ekz>
        Knut iris en sian lo&gcirc;ejon kaj <tld/>is sian tornistron
        <fnt><bib>Fab3</bib>, <lok>Sub la saliko</lok></fnt>;
      </ekz>
      <ekz>
        najbaraj familioj judaj, devigitaj <tld/>i siajn valizojn kaj
        iri al la &leftquot;forirejo&#x201D;, por transporto
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2006/008784.php">Taglibroj
           de murdita infano</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_p;&c_a;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_k;&c_l;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_c;&c_ju;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_o;&c_p;&c_a;&c_k;&c_o;&c_v;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>balit</trd>,
    <trd>shrnovat</trd>,
    <trd>zahu&scaron;&tcaron;ovat <klr>(soubor)</klr></trd>
  </trdgrp>
  <trd lng="de">packen</trd>
  <trd lng="en">pack</trd>
  <trd lng="es">empaquetar</trd>
  <trdgrp lng="fr">
    <trd>emballer</trd>,
    <trd>empaqueter</trd>
  </trdgrp>
  <trd lng="hu">csomagol</trd>
  <trdgrp lng="ja">
    <trd>&#x5305;&#x3080;  <pr>&#x304F;&#x308B;&#x3080;</pr></trd>,
    <trd>&#x5305;&#x88C5;&#x3059;&#x308B;  <pr>&#x307B;&#x3046;&#x305D;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x8377;&#x9020;&#x308A;&#x3059;&#x308B;  <pr>&#x306B;&#x3065;&#x304F;&#x308A;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5727;&#x7E2E;&#x3059;&#x308B;  <pr>&#x3042;&#x3063;&#x3057;&#x3085;&#x304F;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>pakken</trd>,
    <trd>inpakken</trd>
  </trdgrp>
  <trd lng="pl">pakowa&cacute; si&eogonek;</trd>
  <trdgrp lng="pt">
    <trd>embrulhar</trd>,
    <trd>empacotar</trd>,
    <trd>enfardar</trd>,
    <trd>emalar</trd>,
    <trd>empalhar</trd>
  </trdgrp>
  <trd lng="ro">&#x219;i face bagajul</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_p;&c_a;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>komprimova&tcaron; <klr>(s&uacute;bor)</klr></trd>,
    <trd>zahus&tcaron;ova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_a;&#x43A;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_u;&#x43F;&c_a;&#x43A;&#x43E;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_t;&#x44E;&#x43A;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_u;&c_v;&#x2019;&#x44F;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i; &c_v; &c_t;&#x44E;&#x43A;&c_i;</trd>,
    <trd>&c_u;&#x43A;&#x43B;&c_a;&c_d;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="pak.0o">
  <kap><tld/>o</kap>
  <snc mrk="pak.0o.KOMUNE">
    <dif>
      Aro da <tld/>itaj objektoj:
      <ekz>
        <klr>[li]</klr> portas sur la &scirc;ultro <tld/>on da tolo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Sesope oni trairas la tutan mondon</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>pli kostas la sako ol la tuta <tld/>o</ind>
        <trd lng="hu">t&ouml;bbe ker&uuml;l a h&uacute;s, mint a leves</trd>
        (<ref tip="vid" cel="kandel.0o">kandelo</ref>)
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ku&scirc;ante sur sakoj kaj <tld/>oj, en krakanta veturilo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        ligu la <tld/>on per la speciala bendo, fermu ties ekstrema&jcirc;ojn
        per sigelvakso
        <fnt>
          <bib>Monato</bib>
          <aut>Filippo</aut>
          <vrk><url ref="&Monato;/2006/008898.php">Moderna
          po&scirc;to</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        sendi <tld/>on eksterlanden
        <fnt>
          <bib>Monato</bib>
          <aut>Filippo</aut>
          <vrk><url ref="&Monato;/2006/008898.php">Moderna
          po&scirc;to</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_k;&c_je;&c_t;</trd>,
    <trd>&c_u;&c_p;&c_a;&c_k;&c_o;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_c;&c_ju;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_a;&c_k;&c_je;&c_t;</trd>
  <trd lng="bo">&#xF66;&#xF92;&#xFB2;&#xF72;&#xF63;&#xF0B;&#xF60;&#xF50;&#xF74;&#xF58;&#xF0B;</trd>
  <trdgrp lng="de">
    <trd>Paket</trd>,
    <trd>Pack <klr>(Gep&auml;ck)</klr></trd>,
    <trd>Gep&auml;ck</trd>
  </trdgrp>
  <trd lng="en">package</trd>
  <trd lng="es">paquete</trd>
  <trd lng="fr">paquet</trd>
