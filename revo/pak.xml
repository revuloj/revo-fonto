<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc><rad>pak</rad>/i
    </kap>

    <drv mrk="pak.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Kunaran&gcirc;i plej malvaste iajn objektojn, &ccirc;u ilin ligante
          per &scirc;nuro, &ccirc;u envolvante en paperon a&ubreve;
          &scirc;tofon, &ccirc;u en&scirc;ovante en sakon, &ccirc;u enpremante
          en kofron a&ubreve; valizon, &ccirc;u ordigante en kesto:
          <ekz>
            li <tld/>is sian hava&jcirc;on<fnt>Z</fnt>;
          </ekz><ekz>
            la komerca&jcirc;oj devas esti <tld/>ataj<fnt>Z</fnt>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            li estis <tld/>ita kun ok aliaj personoj en la interno
            de po&scirc;ta kale&scirc;o<fnt>Z</fnt>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            ni estis prem<tld/>itaj en la &ccirc;ambro.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_p;&c_a;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_u;&c_p;&c_a;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_u;&c_k;&c_l;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_c;&c_ju;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
      </trdgrp>
      <trd lng="cs">balit</trd>
      <trd lng="de">packen</trd>
      <trd lng="en">pack</trd>
      <trd lng="hu">csomagol</trd>  
      <trdgrp lng="nl">
        <trd>pakken</trd>,
        <trd>inpakken</trd>
      </trdgrp>
      <trdgrp lng="pt">
        <trd>embrulhar</trd>,
        <trd>empacotar</trd>,
        <trd>enfardar</trd>,
        <trd>emalar</trd>,            
        <trd>empalhar</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_p;&c_a;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_u;&c_p;&c_a;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
    </drv>

    <drv mrk="pak.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Aro da <tld/>itaj objektoj:
          <ekz>
            <ind>pli kostas la sako ol la tuta
            <tld/>o</ind>(<ref tip="vid" cel="kandel.0o">kandelo</ref>)
            <fnt><bib>PrV</bib></fnt>;
            <trd lng="hu">t&ouml;bbe ker&uuml;l a h&uacute;s, mint a leves</trd>
            </ekz>
            <ekz>
            mi dissigelis la <ind><tld/>eto</ind>n, kiun ili kun si tre
            garde
            portis<fnt>Z</fnt>.
            <trdgrp lng="hu">
              <trd>kis <ind>csomag</ind></trd>,
              <trd>csomagocska</trd>
            </trdgrp>
            <trd lng="ru">&c_p;&c_a;&c_k;&c_je;&c_t;</trd>
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_p;&c_a;&c_k;&c_je;&c_t;</trd>,
        <trd>&c_u;&c_p;&c_a;&c_k;&c_o;&c_w;&c_k;&c_a;</trd>,
        <trd>&c_c;&c_ju;&c_k;</trd>
      </trdgrp>
      <trd lng="hu">csomag</trd>  
      <trd lng="nl">pak</trd>
      <trdgrp lng="ru">
        <trd>&c_p;&c_a;&c_k;&c_je;&c_t;</trd>,
        <trd>&c_u;&c_p;&c_a;&c_k;&c_o;&c_v;&c_k;&c_a;</trd>.
        <trd>&c_t;&c_ju;&c_k;</trd>
      </trdgrp>
    </drv>

    <drv mrk="pak.0ajxo">
      <kap><tld/>a&jcirc;o</kap>
      <snc mrk="pak.0ajxo.unupeco">
        <ref tip="dif" cel="pak.0o"><tld/>o</ref>
        <ekz>
          Jim elprenis <tld/>a&jcirc;on el sia surtuta po&scirc;o kaj &jcirc;etis
          &gcirc;in sur la tablon
          <fnt>
            <aut>O. Henry, tr. Donald J. Harlow</aut>:
            <vrk>La donaco de la magoj</vrk>.
            <bib>LOdE</bib>, <lok>2001. N-ro 12</lok>
          </fnt>;
        </ekz><ekz>
          legantoj de MONATO, kiuj volas helpi forigi malsaton inter
          la infanoj de la mondo, povas fari same kaj a&ccirc;eti tiajn
          <tld/>a&jcirc;ojn kun 30 kompletaj man&gcirc;oj pere de Flandra
          Esperanto-Ligo kontra&ubreve; po 31 e&ubreve;roj
          <fnt>
            <aut>Paul PEERAERTS</aut>:
            <vrk>Nova ha&ubreve;to por la a&ubreve;to-&ccirc;efo</vrk>.
            <bib>Monato</bib>, <lok>2002/11, p.14</lok>
          </fnt>.
        </ekz>
        <trdgrp lng="nl">
          <trd>pak</trd>,
          <trd>verpakking</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_p;&c_a;&c_k;&c_je;&c_t;</trd>,
          <trd>&c_u;&c_p;&c_a;&c_k;&c_o;&c_v;&c_k;&c_a;</trd>.
          <trd>&c_t;&c_ju;&c_k;</trd>
        </trdgrp>
      </snc>
      <snc mrk="pak.0ajxo.bagagxo">
        <dif>
          <ref tip="dif" cel="bagagx.0o">Baga&gcirc;o</ref>:
          <ekz>
            kiam mi veturas, mi neniam prenas kun mi multon da
            <tld/>a&jcirc;o<fnt>Z</fnt>;
          </ekz><ekz>
            forportu vian <tld/>a&jcirc;on kaj hava&jcirc;on
            <klr>(definitive foriru)</klr>
            <fnt><bib>PrV</bib></fnt>.</ekz>
        </dif>
        <trd lng="be">&c_b;&c_a;&c_g;&c_a;&c_zh;</trd>
        <trd lng="de">Gep&auml;ck</trd>
        <trd lng="hu">poggy&aacute;sz</trd>  
        <trd lng="nl">bagage</trd>
        <trd lng="ru">&c_b;&c_a;&c_g;&c_a;&c_zh;</trd>
      </snc>
    </drv>

    <drv mrk="pak.0umo">
      <kap><tld/>umo<fnt>PIV1</fnt></kap>
      <snc>
        <dif>
          Entenanta&jcirc;o, kovra&jcirc;o uzata por <tld/>i:
          <ekz>
            kvankam <tld/>umo estas vesto de varo kaj povas beligi
            &gcirc;in, tamen por simpla varo troa <tld/>umo ne nur pliigas
            la koston, sed anka&ubreve; ka&ubreve;zas grandan mal&scirc;paron de
            naturaj ri&ccirc;fontoj
            <fnt>
              <aut>MU Binghua</aut>:
              <vrk>Arbarego por pakumi &ccirc;emizojn </vrk>.
              <bib>Monato</bib>
            </fnt>.
          </ekz>
        </dif>
        <ref tip="vid" cel="tar.0o">taro</ref>
        <trd lng="de">Verpackung</trd>
        <trdgrp lng="hu">
          <trd>csomagol&aacute;s<klr tip="ind">(g&ouml;ngy&ouml;leg)</klr></trd>,
          <trd>csomagol&oacute;anyag</trd>,
          <trd>g&ouml;ngy&ouml;leg</trd>
        </trdgrp>
        <trd lng="nl">verpakking</trd>
        <trdgrp lng="ru">
          <trd>&c_u;&c_p;&c_a;&c_k;&c_o;&c_v;&c_k;&c_a;</trd>,
          <trd>&c_t;&c_a;&c_r;&c_a;</trd>
        </trdgrp>
      </snc>
    </drv>

    <drv mrk="pak.mal0i">
      <kap>el<tld/>i<fnt>Z</fnt>, mal<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Elmeti el <tld/>o.
        </dif>
      </snc>
      <trd lng="be">&c_r;&c_a;&c_s;&c_p;&c_a;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
      <trd lng="de">auspacken</trd>
      <trd lng="hu">kicsomagol</trd>  
      <trd lng="nl">uitpakken</trd>
      <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_a;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </drv>

    <drv mrk="pak.en0i">
      <kap>en<tld/>i<fnt>Z</fnt></kap>
      <snc>
        <dif>
          Meti en <tld/>on..
        </dif>
      </snc>
      <trd lng="be">&c_u;&c_p;&c_a;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
      <trd lng="de">einpacken</trd>
      <trd lng="hu">becsomagol</trd>  
      <trd lng="nl">inpakken</trd>
      <trd lng="ru">&c_u;&c_p;&c_a;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </drv>
  </art>
