    <trd>lezione<klr> (consiglio)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x52E7;&#x544A;  <pr>&#x304B;&#x3093;&#x3053;&#x304F;</pr></trd>,
    <trd>&#x52E7;&#x3081;  <pr>&#x3059;&#x3059;&#x3081;</pr></trd>,
    <trd>&#x8A13;&#x6212;  <pr>&#x304F;&#x3093;&#x304B;&#x3044;</pr></trd>,
    <trd>&#x8AAC;&#x8AED;  <pr>&#x305B;&#x3064;&#x3086;</pr></trd>,
    <trd>&#x53F1;&#x8CAC;  <pr>&#x3057;&#x3063;&#x305B;&#x304D;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>admonicja</trd>,
    <trd>napomnienie</trd>
  </trdgrp>
  <trd lng="ro">admonestare</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_v;&c_je;&c_shch;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_u;&c_g;&c_o;&c_v;&c_o;&c_r;&c_y;</trd>
  </trdgrp>
  <trd lng="sk">napomenutie</trd>
</drv>

<drv mrk="admon.de0i">
  <kap>de<tld/>i</kap>
  <snc>
    <dif>
      <tld lit="A"/>i ne fari, <tld/>i al la malo:
      <ekz>
        la &scirc;ajne senkulpa vorteto &leftquot;<esc>aliel</esc>&rightquot; kunportas
        tiom da &scirc;an&gcirc;oj, ke oni devas strikte de<tld/>i
        de &gcirc;ia uzo
