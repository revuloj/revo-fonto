    <trdgrp lng="zh">
    <trd>&#x4FDB; <pr>mi&#x1CE;n</pr></trd>,
    <trd>&#x52B1; <pr>l&#xEC;</pr></trd>,
    <trd>&#x52F5; <pr>l&#xEC;</pr></trd>,
    <trd>&#x529D;&#x8BEB; <pr>qu&#xE0;nji&#xE8;</pr></trd>,
    <trd>&#x52F8;&#x8AA1; <pr>qu&#xE0;nji&#xE8;</pr></trd>,
    <trd>&#x529D;&#x8C0F; <pr>qu&#xE0;nji&#xE0;n</pr></trd>,
    <trd>&#x52F8;&#x8AEB; <pr>qu&#xE0;nji&#xE0;n</pr></trd>,
    <trd>&#x529D; <pr>qu&#xE0;n</pr></trd>,
    <trd>&#x52F8; <pr>qu&#xE0;n</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="bg">
    <trd>&c_u;&c_b;&c_je;&c_zh;&c_d;&c_a;&c_v;&c_a;&c_m;</trd>,
