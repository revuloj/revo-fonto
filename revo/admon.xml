    <trd>&#x5F37;&#x304F;&#x52E7;&#x3081;&#x308B;  <pr>&#x3064;&#x3088;&#x304F;&#x3059;&#x3059;&#x3081;&#x308B;</pr></trd>,
    <trd>&#x6212;&#x3081;&#x308B;  <pr>&#x3044;&#x307E;&#x3057;&#x3081;&#x308B;</pr></trd>,
    <trd>&#x8A13;&#x6212;&#x3059;&#x308B; <pr>&#x304F;&#x3093;&#x304B;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x8AAC;&#x8AED;&#x3059;&#x308B;  <pr>&#x305B;&#x3064;&#x3055;&#x3068;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x53F1;&#x8CAC;&#x3059;&#x308B;  <pr>&#x3057;&#x3063;&#x305B;&#x304D;&#x3059;&#x308B;</pr></trd>
