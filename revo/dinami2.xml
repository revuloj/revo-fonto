<drv mrk="dinami2.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Eksploda <ref tip="super" cel="substa.0o.FIZ">substanco</ref>,
      konsistanta el <ref tip="prt" cel="nitrog1.0o">nitroglicerino</ref>
      kaj ne&ubreve;tra substanco:
      <ekz>
        post sia unua patento li kolektis pli ol 300 patentojn plejparte rilate <tld/>on
        kaj la sendan&gcirc;eran uzon de &gcirc;i
        <fnt>
          <bib>Monato</bib>
          <aut>Edward Kusters</aut>
          <vrk><url ref="&Monato;/2006/009100.php">Alfred Nobel: nobelo inter la sciencistoj kaj lingvistoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_n;&c_a;&c_m;&c_ib;&c_t;</trd>
  <trd lng="bg">&c_d;&c_i;&c_n;&c_a;&c_m;&c_i;&c_t;</trd>
