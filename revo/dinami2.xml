  <trd lng="cs">dynamit</trd>
  <trd lng="de">Dynamit</trd>
  <trd lng="en">dynamite</trd>
  <trd lng="es">dinamita</trd>
  <trd lng="fa">&a_d;&f_y;&a_n;&a_A;&a_m;&f_y;&a_t;</trd>
  <trd lng="fr">dynamite</trd>
  <trd lng="he">&#x5D3;&#x5B4;&#x5D9;&#x5E0;&#x5B8;&#x5DE;&#x5B4;&#x5D9;&#x5D8;</trd>
  <trd lng="hu">dinamit</trd>
  <trd lng="pl">dynamit</trd>
  <trd lng="pt">dinamite</trd>
  <trd lng="ru">&c_d;&c_i;&c_n;&c_a;&c_m;&c_i;&c_t;</trd>
  <trd lng="sk">dynamit</trd>
  <trd lng="sv">dynamit</trd>
</drv>
