<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: bogot.xml,v 1.5 2023/08/31 08:48:52 afido Exp $">
    <kap>
      <rad>Bogot</rad>/o
    </kap>

<drv mrk="bogot.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      La <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&ccirc;efurbo</ref>
      de <ref tip="malprt" cel="kolombi.K0o">Kolombio</ref>:
      <ekz>
        trajno el <tld/>o
        <fnt>
          <vrk><url ref="https://www.youtube.com/watch?v=FKADsXDCHhw">La trajno el Bogoto | Kelkaj
          Rakontoj Revenas</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi eliris el <tld/>o kaj renkontis Esperantiston en <nom>Cali</nom>
        <fnt>
          <vrk><url ref="https://sites.google.com/site/mondvojago6/esperanto/esperanto-1/115eshtm">Mondvoja&gcirc;o</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o kaj Va&scirc;ingtono asertas ke la sep militbazoj disponigitaj al Usono sur la
        kolombia teritorio, la&ubreve; la interkonsento subskribita la 30-an de oktobro
        2009, celas fortigi la batalon kontra&ubreve; la drogkomerco
        <fnt>
          <vrk><url ref="&Mondediplo;article1576.html">Va&scirc;ingtono reaktivigas la
          Monroe-doktrinon</url></vrk>
          <lok>Le Monde diplomatique' en Esperanto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_B;&c_a;&c_g;&c_a;&c_t;&c_a; <klr>(&c_s;&c_t;&c_a;&c_l;&c_ib;&c_c;&c_a; &c_K;&c_a;&c_l;&c_ju;&c_m;&c_b;&c_ib;&c_ib;)</klr></trd>
    <trd lng="de">Bogot&aacute;</trd>
    <trd lng="en">Bogot&aacute;</trd>
    <trd lng="fr">Bogota</trd>
  </snc>
  <trd lng="ja">&#x30DC;&#x30B4;&#x30BF;</trd>
  <trd lng="uk">&c_B;&#x43E;&c_gu;&#x43E;&c_t;&c_a;</trd>
</drv>

  </art>

