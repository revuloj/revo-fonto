<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>jup</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="jup.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="jup.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="vest.0o.TEKS">Vesto</ref> kovranta la 
      malsupran parton de la korpo, &ccirc;irka&ubreve;pendante de la talio: 
      <ekz>
        la <tld/>oj pro modo mallongi&gcirc;is;
      </ekz>
      <ekz>
        <tld/>a regado
        <klr>(edzina regado &ndash; jupo en pluraj kulturoj estas portata 
        nur de inoj)</klr> .
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_a;&c_d;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="br">brozh</trd>
  <trd lng="de">Rock</trd>
  <trd lng="el">&phi;&omikron;&ypsilon_ton;&sigma;&tau;&alfa;</trd>
  <trd lng="en">skirt</trd>
  <trd lng="es">falda</trd>
  <trd lng="hu">szoknya</trd>
  <trd lng="it">gonna</trd>
  <trd lng="fr">jupe</trd>
  <trd lng="nl">rok</trd>
  <trd lng="pt">saia</trd>
  <trd lng="ru">&c_ju;&c_b;&c_k;&c_a;</trd>
  <trd lng="sv">kjol</trd>
</drv>

<drv mrk="jup.sub0o">
  <kap>sub<tld/>o</kap>
  <snc mrk="jup.sub0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <tld lit="J"/>o portata sub la ekstera <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="br">
    <trd>brozh-dindan</trd>,
    <trd>goueledenn</trd>,
    <trd>lostenn-dindan</trd>
  </trdgrp>
  <trd lng="de">Unterrock</trd>
  <trd lng="fr">jupon</trd>
  <trd lng="hu">als&oacute;szoknya</trd>
  <trd lng="nl">onderrok</trd>
  <trd lng="ru">&c_n;&c_i;&c_zh;&c_n;&c_ja;&c_ja; &c_ju;&c_b;&c_k;&c_a;</trd>
  <trd lng="sv">underkjol</trd>
</drv>
</art>
