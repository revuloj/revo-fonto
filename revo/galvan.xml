<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>galvan</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="galvan.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      Rilatanta al elektra kurento produktita de pilo a&ubreve; al io
      produktata de tia kurento:
      <ekz><tld/>a elektro, kurento.</ekz>
    </dif>
  </snc>
  <trd lng="de">galvanisch</trd>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
<drv mrk="galvan.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      <tld lit="G"/>a kli&scirc;o reproduktita reliefe per <tld/>oplastiko. 
    </dif>
  </snc>
  <trd lng="de">Druckplatte</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_n;&c_o;</trd>,
    <trd>&c_g;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_n;&c_o;&c_p;&c_l;&c_a;&c_s;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; &c_k;&c_l;&c_i;&c_sh;&c_je;</trd>
  </trdgrp>
</drv>
<drv mrk="galvan.0ismo">
  <kap><tld/>ismo</kap>
  <snc num="1">
    <dif>
      Elektraj fenomenoj produktataj de &hcirc;emia energio. 
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Parto de la elektroscienco, kiu pritraktas fenomenojn
      de la <tld/>a fluo.  
    </dif>
  </snc>
  <trd lng="de">Galvanismus</trd>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_n;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
