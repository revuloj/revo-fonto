<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>galvan</rad>/a
<fnt>Z</fnt></kap>
<drv mrk="galvan.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      Rilatanta al <ref cel="kurent.kontinua0o">kontinua kurento</ref>,
      ekz-e produktata de pilo, a&ubreve; al io produktata de tia kurento:
      <ekz>
        <tld/>a elektro, kurento, efiko.
      </ekz>
    </dif>
    <ref tip="vid" cel="galvan6.0o">galvanismo.</ref>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_n;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="de">galvanisch</trd>
  <trd lng="el">&gamma;&alfa;&lambda;&beta;&alfa;&ny;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">galvanic</trd>
  <trd lng="es">galv&aacute;nico</trd>
  <trd lng="fr">galvanique</trd>
  <trdgrp lng="hu">
    <trd>galv&aacute;n-</trd>,
    <trd>galvanikus</trd>
  </trdgrp>
  <trd lng="nl">galvanisch</trd>
  <trd lng="pt">galv&acirc;nico</trd>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="galvan.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">TIP</uzo>
  <snc>
    <dif>
      <tld lit="G"/>a kli&scirc;o reproduktita reliefe per
      <ref cel="galvan3.0o">galvanoplastiko</ref>.
    </dif>
  </snc>
  <adm>
    Neklara difino. Kio estas &leftquot;~a kli&scirc;o&rightquot;? &Scirc;ajnas, ke &leftquot;klisxo&rightquot; sufi&ccirc;us: ja
    &gcirc;i estas ~a, &ccirc;ar produktita per galvanoplastiko. Por kio servas tia
    kli&scirc;o?
    <aut>MB</aut>
  </adm>
  <trd lng="de">Druckplatte</trd>
  <trdgrp lng="fr">
    <trd>galvano</trd>,
    <trd>galvanotype</trd>
  </trdgrp>
  <trd lng="hu">galvanot&iacute;pia<klr tip="ind"> (term&eacute;k)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_n;&c_o;</trd>,
    <trd>&c_g;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_n;&c_o;&c_p;&c_l;&c_a;&c_s;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; &c_k;&c_l;&c_i;&c_sh;&c_je;</trd>
  </trdgrp>
</drv>
</art>
