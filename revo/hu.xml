<vortaro>
<art mrk="$Id: hu.xml,v 1.14 2022/03/20 17:24:56 revo Exp $">
<kap>
  <rad>hu</rad><fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="hu.0">
  <kap><tld/></kap>
  <gra><vspec>ekkrio</vspec></gra>
  <snc mrk="hu.0.KOMUNE">
    <dif>
      Ekkrio por inciti a&ubreve; insulte timigi, ofte &scirc;erce.
    </dif>
  </snc>
  <trd lng="de">buh</trd>
  <trd lng="en">boo</trd>
  <trdgrp lng="es">
    <trd>&#xA1;hu!</trd>,
    <trd>&#xA1;uh!</trd>
  </trdgrp>
  <trd lng="nl">boe</trd>
  <trdgrp lng="fr">
    <trd>ouh</trd>,
    <trd>bouh</trd></trdgrp> 
  <trdgrp lng="pt">
    <trd>hu</trd>,
    <trd>uh</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;</trd>,
    <trd>&c_f;&c_u;</trd>
  </trdgrp>
</drv>

<drv mrk="hu.0i">
  <kap><tld/>i,
  <var><kap><tld/>adi</kap></var></kap>
  <snc mrk="hu.0i.KOMUNE">
    <dif>
      Krii &leftquot;<tld/>&rightquot;:
      <ekz>
        <tld/>adas aliaj infanoj
        <fnt>
          <aut>B. Vian, trad. M. Corobu</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/loic.roussain/236630">
          La Mallernanto</url></vrk>,
          <lok>2010-03-16</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">abuchear</trd>
  <trd lng="fr">huer</trd>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x445;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43E;&#x439;&#x43A;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x441;&#x442;&#x440;&#x430;&#x448;&#x438;&#x442;&#x438; &#x43A;&#x43E;&#x433;&#x43E;-&#x43D;. (&#x43A;&#x440;&#x438;&#x43A;&#x43E;&#x43C;)</trd>
  </trdgrp>
</drv>

<drv mrk="hu.0o">
  <kap><tld/>o,
  <var><kap><tld/>ado</kap></var></kap>
  <snc mrk="hu.0o.KOMUNE">
    <dif>
      &leftquot;<tld lit="H"/>&rightquot;-kriado:
      <ekz>
        la ripozpa&ubreve;zon la dancantoj aklamis per
        protestfajfoj kaj <tld/>adoj
        <fnt><aut>S. Larbar</aut>, <vrk>Karuseloj</vrk>, 
        <lok>1987</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">abucheo</trd>
  <trd lng="fr">hu&eacute;e</trd>
</drv>

<drv mrk="hu.00">
  <kap><tld/>&nbsp;<tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="hu.00.SON">
    <dif>
      <ref tip="lst" cel="blek.0o" lst="voko:blekoj">Bleko</ref>
      de strigo:
      <ekz>
        <ctl><tld/>-<tld/>, <tld/>-<tld/>!</ctl>
        <klr>[la strigoj]</klr> amba&ubreve; trumpetis
        <fnt>
          <bib>Fab3</bib>,
          <vrk>Estas tute certe</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">&#xA1;hu-hu!<klr> (grito de la lechuza)</klr></trd>
  <trd lng="fr">hou-hou !<klr> (cri de chouette)</klr></trd>
</drv>
</art>
