<art mrk="$Id: telemetri.xml,v 1.9 2024/01/16 01:35:14 revo Exp $">
<kap>
  <rad>telemetri</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="telemetri.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc mrk="telemetri.0o.TEK">
    <dif>
      <ref tip="dif" cel="telemezur.0o.TEK">Telemezuro</ref>:
      <ekz>
        en 2007 estis farita mapo de Danlando per <tld/>o
        <fnt>
          <aut>A. Lucas</aut>
          <vrk><url
          ref="https://aktuale.scienca-revuo.info/2017/10/27/nova-vikinga-citadelo-malkovrita/">
          Nova Vikinga Citadelo malkovrita</url></vrk>
          <lok>Aktuala&jcirc;oj de Scienca Revuo, 2017-10-27</lok>
        </fnt>;
      </ekz>
      <ekz>
        la migrovojo estis &jcirc;use malka&scirc;ita kaj male al anta&ubreve;aj sugestoj oni montris per
        satelita <tld/>o, ke &gcirc;i estas interna tra la afrika kontinento
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16192">Kliffalko</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_l;&c_je;&c_m;&c_e;&c_t;&c_r;&c_y;&c_ja;</trd>
  <trd lng="de">Telemetrie</trd>
  <trd lng="en">telemetry</trd>
  <trd lng="fr">t&eacute;l&eacute;m&eacute;trie</trd>
  <trd lng="ja">&#36960;&#38548;&#x6E2C;&#x5B9A;  <pr>&#12360;&#12435;&#x304B;&#x304F;&#x305D;&#x304F;&#12390;&#12356;</pr></trd>
  <trd lng="nl">telemetrie</trd>
  <trd lng="pl">telemetria</trd>
  <trd lng="uk">&c_t;&c_je;&#x43B;&c_je;&#x43C;&c_je;&c_t;&c_r;&c_ib;&#x44F;</trd>
</drv>
</art>

