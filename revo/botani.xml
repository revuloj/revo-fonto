<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>botanik</rad>/o
</kap>
<drv mrk="botani.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SCI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="scienc.0o.fako">Scienco</ref>
      pri la <ref tip="prt" cel="plant.0oj">plantoj</ref>,
      subfako de <ref tip="malprt" cel="biolog1.0o">biologio</ref>:
      <ekz>
        la kolektantoj de naturaj specimenoj stimulis la <tld/>on, kiu dum
        la renesanco apartigis sin de la medicino<fnt><aut>Johan Van
        Eenoo</aut>: <vrk>La bru&gcirc;a Centuriae plantarum rariorum retrovita
        en Krakovo</vrk>, <bib>Monato</bib><lok>jaro 1998a, numero 1a, p.
        16a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Botanik</trd>
  <trd lng="es">bot&aacute;nica</trd>
  <trd lng="fr">botanique<klr> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>n&ouml;v&eacute;nytan</trd>,
    <trd>botanika</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_o;&c_t;&c_a;&c_n;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">botanika</trd>
</drv>

<drv mrk="botani.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        antropologiaj, zoologiaj kaj <tld/>aj faktoj pruvas
        tion<fnt><aut>Andr&eacute; Cherpillod</aut>: <vrk>Atlantido la perdita
        kontinento</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero 7a, p.
        21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">botanisch</trd>
  <trdgrp lng="fr">
    <trd>botanique<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>n&ouml;v&eacute;nytani</trd>,
    <trd>botanikai</trd>
  </trdgrp>
</drv>

<drv mrk="botani.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Tiu kiu sin okupas pri
      <ref tip="malprt" cel="botani.0o"><tld/>o</ref>:
      <ekz>
        la <tld/>istoj asertas, ke post 50 jaroj 60 000 plantspecioj estos
        malaperintaj<fnt><aut>Robert Roze</aut>: <vrk>Vivisekcio de
        polenero</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero 11a, p.
        12a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Botaniker</trd>
  <trd lng="es">bot&aacute;nico</trd>
  <trd lng="fr">botaniste</trd>
  <trd lng="hu">botanikus</trd>
  <trd lng="ru">&c_b;&c_o;&c_t;&c_a;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">botanik</trd>
</drv>
</art>
