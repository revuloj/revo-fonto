<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>huŝ</rad>/i
</kap>
<drv mrk="husx.0i">
  <kap><tld/>i</kap>
  <snc mrk="husx.0i.SON">
    <dif>
      <ref tip="super" cel="tim.for0igi.KOMUNE">Fortimigi</ref> dirante
      „<tld/>“:
      <ekz>
        Leo estas ŝia frat', /
        fajfas melodion, /
        krias, <tld/>as al la kat'
        <fnt>
        <aut>Jon Rommesmo</aut>,
        <vrk>Familio kaj sonoj</vrk>,
        <lok><url
 
ref="http://abonu.com/archive/kulturo.123456789/200302/18000505.html">Teflor
o,
        2003-02, numero 4a</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">effrayer<klr> (en criant)</klr></trd>
  <trd lng="pt">enxotar</trd>
</drv>

<drv mrk="husx.0">
  <kap><tld/></kap> <fnt><bib>PIV1</bib></fnt>
  <gra><vspec>bruvorto</vspec></gra>
  <snc mrk="husx.0.KOMUNE">
    <dif>
      Krio fortimiga:
      <ekz>
        kiam la flava kokino volis enpaŝi, la malalta servistino kriis
        „<tld lit="H"/>!“ kaj skuis sian antaŭveston
        <fnt>
        <aut>L. Frank Baum, trad. D. Broadribb</aut>,
        <vrk><url
        ref="http://esperanto.org/esperante/literatur/Baum/ozma/">Ozma de
        Oz</url></vrk>,
        <lok>6, La Kapoj de Langvorlino. - 1996</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="pl">
  	<trd>kysz!</trd>
  	<trd>sio!</trd>
  </trdgrp>
  <trd lng="pt">xô</trd>
  <refgrp tip="vid">
    <ref cel="for.0.malapero">for <sncref/></ref>
  </refgrp>
  <trd lng="fr">ouste !</trd>
</drv>
</art>

