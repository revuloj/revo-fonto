<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>delfen</rad>/o
</kap>
<drv mrk="delfen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="delfen.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Delphinus</trd>), kun beksimila bu&scirc;o.
    </dif>
  </snc>
  <trd lng="de">Delphin</trd>
  <trd lng="fr">dauphin<klr tip="ind"> (animal)</klr></trd>
  <trd lng="ru">&c_d;&c_je;&c_l;&c_mol;&c_f;&c_i;&c_n;</trd>
</drv>
<drv mrk="delfen.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="cetac.0oj">cetacoj</ref>
      (<trd lng="la">Delphinidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Delphine <klr tip="ind">(Familie)</klr></trd>
  <trd lng="fr">delphinid&eacute;s</trd>
</drv>
</art>
