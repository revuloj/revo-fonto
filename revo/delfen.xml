<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: delfen.xml,v 1.34 2022/09/19 13:06:40 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>delfen</rad>/o
</kap>
<drv mrk="delfen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="delfen.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Iu el <ref tip="malprt" cel="cetac.0oj">cetacoj</ref>
      havanta dentojn, dorsan na&gcirc;ilon kaj nur unu blovtruon
      (<trd lng="la">Odontoceti</trd>):
      <ekz>
        same kiel balenoj kaj <tld/>oj ili estas mamuloj
        <fnt>
        <aut>W. M. A. De Smet</aut>,
        <vrk>Gigantoj sur strandoj</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 1998a, numero 11a, p. 22a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Zahnwal</trd>
  </snc>
  <snc mrk="delfen.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="cetac.0o" lst="voko:cetacoj">Cetaco</ref> 
      kun beksimila bu&scirc;o;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref>
       el familio <ref tip="super" cel="delfen1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Delphinus</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_l;&c_mol;&c_f;&c_ib;&c_n;</trd>
  <trd lng="bg">&c_d;&c_je;&c_l;&c_f;&c_i;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>delf&iacute;n obecn&yacute;</trd>,
    <trd>pl&iacute;skavice obecn&aacute;</trd>
  </trdgrp>
  <trd lng="de">Delphin</trd>
  <trd lng="en">dolphin</trd>
  <trd lng="es">delf&iacute;n</trd>
  <trd lng="fr">dauphin<klr tip="ind"> (animal)</klr></trd>
  <trd lng="hu">delfin</trd>
  <trd lng="id">lumba-lumba</trd>
  <trd lng="nl">dolfijn</trd>
  <trd lng="pl">delfin</trd>
  <trdgrp lng="pt">
    <trd>golfinho</trd>,
    <trd>delfim</trd>,
    <trd>boto</trd>
  </trdgrp>
  <trd lng="ro">delfin</trd>
  <trd lng="ru">&c_d;&c_je;&c_l;&c_mol;&c_f;&c_i;&c_n;</trd>
  <trd lng="sk">delf&iacute;n</trd>
</drv>

<drv mrk="delfen.D0o">
  <kap><tld lit="D"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
  </kap>
  <mlg>Del</mlg>
  <snc mrk="delfen.D0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Nord-hemisfera <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref> (<trd
      lng="la">Delphinus</trd>).
    </dif>
    <trd lng="be">&c_D;&c_e;&c_l;&c_mol;&c_f;&c_ib;&c_n; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#8217;&c_je;)</klr></trd>
  <trd lng="de">Delfin</trd>
  <trd lng="en">Delphinus</trd>
  <trd lng="es">Delf&iacute;n</trd>
  <trd lng="fr">Dauphin<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="pl">Delfin  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Delfinul  <klr>(constela&#539;ie)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>delf&iacute;n obecn&yacute;</trd>,
    <trd>pl&iacute;skavice obecn&aacute;</trd>
  </trdgrp>
  <trd lng="he">&#x5D3;&#x5D5;&#x5DC;&#x5E4;&#x5D9;&#x5DF;</trd>
  <trd lng="sk">delf&iacute;n</trd>
  <trd lng="uk">&c_d;&c_je;&#x43B;&#x44C;&c_f;&c_ib;&#x43D;</trd>
</drv>

<drv mrk="delfen.river0o">
  <kap>river<tld/>o</kap>
  <snc mrk="delfen.river0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Iu el familio de <ref tip="lst" cel="delfen.0o.KOMUNE" lst="voko:cetacoj"><tld/>oj</ref>
      (<trd lng="la">Iniidae</trd>), &gcirc;is 3&nbsp;m longa, kun malalta 
      dorsna&gcirc;ilo, tre fleksebla kapo kaj pin&ccirc;iletoforma bu&scirc;o kun &gcirc;is 200
      dentoj,
      vivanta en tropikaj riveroj: 
      <ekz>
        min timigis la grandaj river-<tld/>oj, kiuj kelkfoje ekaperis
        tre proksime al nia ne tre longa boato
        <fnt><aut>W. Schad</aut>,
        <vrk>Orinoko-delto: Indianoj, mar&ccirc;oj, arbarego</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2000a, numero 11a, p. 20a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Flussdelfin</trd>
    <trd lng="id"><ind>lumba-lumba</ind> sungai</trd>
    <trd lng="pl">delfin rzeczny</trd>
    <trd lng="ro">delfinul r&acirc;ului</trd>
  </snc>
 
</drv>
</art>
