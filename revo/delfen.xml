<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>delfen</rad>/o
</kap>
<drv mrk="delfen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="delfen.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Iu el <ref tip="malprt" cel="cetac.0oj">cetacoj</ref>
      havanta dentojn, dorsan na&gcirc;ilon kaj nur unu blovtruon
      (<trd lng="la">Odontoceti</trd>):
      <ekz>
        same kiel balenoj kaj <tld/>oj ili estas mamuloj
        <fnt>
        <aut>W. M. A. De Smet</aut>,
        <vrk>Gigantoj sur strandoj</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 1998a, numero 11a, p. 22a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Zahnwal</trd>
  </snc>
  <snc mrk="delfen.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="cetac.0o" lst="voko:cetacoj">Cetaco</ref> 
      kun beksimila bu&scirc;o;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref>
       el familio <ref tip="super"  cel="delfen1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Delphinus</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_l;&c_mol;&c_f;&c_ib;&c_n;</trd>
  <trd lng="bg">&c_d;&c_je;&c_l;&c_f;&c_i;&c_n;</trd>
  <trd lng="de">Delphin</trd>
  <trd lng="en">dolphin</trd>
  <trd lng="es">delf&iacute;n</trd>
  <trd lng="fr">dauphin<klr tip="ind"> (animal)</klr></trd>
  <trd lng="nl">dolfijn</trd>
  <trd lng="hu">delfin</trd>
  <trd lng="pl">delfin</trd>
  <trdgrp lng="pt">
    <trd>golfinho</trd>,
    <trd>delfim</trd>,
    <trd>boto</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_je;&c_l;&c_mol;&c_f;&c_i;&c_n;</trd>
  <trd lng="sk">delf&iacute;n</trd>
</drv>

<drv mrk="delfen.D0o">
  <kap><tld lit="D"/>o</kap>
  <mlg>Del</mlg>
  <snc mrk="delfen.D0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Nord-hemisfera <ref tip="lst"
      cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref> (<trd
      lng="la">Delphinus</trd>).
    </dif>
  <trd lng="de">Delfin</trd>
  <trd lng="en">Delphinus</trd>
  <trd lng="es">Delf&iacute;n</trd>
  <trd lng="fr">Dauphin<klr> (constellation)</klr></trd>
  <trd lng="pl">Delfin</trd>
  </snc>
</drv>

<drv mrk="delfen.river0o">
  <kap>river<tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="delfen.river0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Na&gcirc;a mamulo proksimume <tld/>granda, vivanta en tropikaj
      riveroj: 
      <ref tip="sub" cel="dugong.0o.ZOO">dugongo</ref>, <ref tip="sub" 
      cel="manat.0o.ZOO">manato</ref>:
      <ekz>
        min timigis la grandaj river-<tld/>oj, kiuj kelkfoje ekaperis
        tre proksime al nia ne tre longa boato
        <fnt><aut>W. Schad</aut>,
        <vrk>Orinoko-delto: Indianoj, mar&ccirc;oj, arbarego</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2000a, numero 11a, p. 20a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Manati</trd>
  <trd lng="en">manatee</trd>
  <trdgrp lng="fr">
    <trd>dugong</trd>,
    <trd>lamantin</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>man&aacute;ti</trd>,
    <trd>lamantin</trd>
  </trdgrp>
  <trd lng="nl">zeekoe</trd>
  <trdgrp lng="pl">
    <trd>manat</trd>,
    <trd>brzegowiec</trd>,
    <trd>lamantyna</trd>
  </trdgrp> 
  <trd lng="ru">&c_l;&c_a;&c_m;&c_a;&c_n;&c_t;&c_i;&c_n;</trd>
</drv>
</art>
