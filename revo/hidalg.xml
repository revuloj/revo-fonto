<vortaro>
<art mrk="$Id: hidalg.xml,v 1.14 2025/03/30 11:03:08 revo Exp $">
<kap>
  <rad>hidalg</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="hidalg.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="nobel.0o">Nobelo</ref> de plej malalta
      rango:
      <ekz>
        en vila&gcirc;o de La Mancha, kies nomon mi ne volas memori, anta&ubreve;
        nelonge vivis <tld/>o<fnt>
        <bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 1a, p.
        27a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Anka&#365; eblas diri <ke>hispana nobelo</ke>.
  </rim>

  <trd lng="be">&c_ib;&c_d;&c_a;&c_l;&c_mol;&c_g;&c_a;</trd>
  <trd lng="bg">&c_h;&c_i;&c_d;&c_a;&c_l;&c_g;&c_o;</trd>
  <trd lng="cs">hidalgo</trd>
  <trd lng="en">hidalgo</trd>
  <trd lng="es">hidalgo</trd>
  <trdgrp lng="fr">
    <trd>hidalgo</trd>,
    <trd>hobereau</trd>
  </trdgrp>
  <trd lng="hu">hidalg&oacute;</trd>
  <trd lng="it">hidalgo</trd>
  <trd lng="nl">hidalgo</trd>
  <trd lng="pt">fidalgo</trd>
  <trd lng="ru">&c_i;&c_d;&c_a;&c_l;&c_mol;&c_g;&c_o;</trd>
  <trd lng="sk">hidalgo</trd>
  <trdgrp lng="uk">
    <trd>&#1075;&#1110;&#1076;&#1072;&#1083;&#1100;&#1169;&#1086;</trd>,
    <trd>&#1110;&#1076;&#1072;&#1083;&#1100;&#1169;&#1086; (&#1110;&#1089;&#1087;&#1072;&#1085;&#1089;&#1100;&#1082;&#1080;&#1081; &#1076;&#1074;&#1086;&#1088;&#1103;&#1085;&#1080;&#1085;)</trd>
  </trdgrp>
</drv>
</art>
