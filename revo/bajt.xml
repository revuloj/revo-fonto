<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>bajt</rad>/o
    </kap>
    <drv mrk="bajt.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">KOMP</uzo>
      <snc>
        <fnt>(<lok><url ref="&KompLeks;/B.html#BAJTO">KompLeks</url>
        </lok>)</fnt>
        <dif>
          Elemento de datumhierarkio, pli granda ol
          <ref cel="bit.0o.KOMP">bito</ref> kaj malpli granda ol
          <ref cel="vort.0o.KOMP">vorto</ref>; praktike, grupo el 8
          bitoj (<ref tip="sub" cel="bit.0oko">bitoko</ref>).
        </dif>
        <ref tip="malprt" cel="datum.0o.KOMP">datumo</ref>
        <trd lng="be">&c_b;&c_a;&c_j;&c_t;</trd>
        <trd lng="de">Byte</trd>
        <trd lng="en">byte</trd>
        <trd lng="es">byte</trd>
        <trd lng="fr">octet</trd>
        <trdgrp lng="hu">
          <trd>b&aacute;jt</trd>,
          <trd>byte</trd>
        </trdgrp>
        <trd lng="nl">byte</trd>
        <trd lng="pl">bajt</trd>
        <trd lng="pt">byte</trd>
        <trd lng="ru">&c_b;&c_a;&c_j;&c_t;</trd>
        <trdgrp lng="sk">
          <trd>bajt</trd>,
          <trd>byte</trd>
        </trdgrp>
      </snc>
    </drv>
  </art>

