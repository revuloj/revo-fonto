<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: drum.xml,v 1.6 2023/11/04 18:50:52 revo Exp $">
<kap>
  <rad>drum</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="drum.0o">
  <kap><tld/>o</kap>
  <snc mrk="drum.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Garnituro de
      <ref tip="super" cel="instru2.frap0o.MUZ">frapinstrumentoj</ref>
      precipe <ref tip="prt" cel="tambur.0o">tamburo</ref>j kaj cimbaloj,
      ludataj de unu homo:
      <ekz>
        la varieco de uzitaj muzikinstrumentoj (violono,
        akordiono, pian-klavaro,
        <tld/>o, kontrabaso, bas-gitaro, mandolino, buzuko,
        citro) tute ne ka&scirc;as la
        agrablan, amikecan basan vo&ccirc;on de <nom>Yvart</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>Alexander Shlafer</aut>
          <vrk><url ref="https://www.monato.be/2010/009749.php">Plia sukceso de Jacques Yvart</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Martin</nom> estas la centra kolono de la grupo,
        kune kun <nom>Johan Lagerdahl</nom> per baso,
        kaj <nom>Matias Levin</nom> per <tld/>o
        <fnt>
          <aut>Vinilkosmo</aut>,
          <vrk><url ref="https://www.facebook.com/Ret.Info/posts/755280751218036:0">
          Martin kaj la talpoj - Superbazaro</url></vrk>,
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Schlagzeug</trd>
  <trd lng="es">bater&iacute;a (m&uacute;sica)</trd>
  <trd lng="fr">batterie<klr> (musique)</klr></trd>
  <trd lng="pl">perkusja</trd>
  <trd lng="uk">&c_u;&c_d;&c_a;&c_r;&c_n;&c_ib; &c_ib;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;&c_i;</trd>
  <trd lng="zh">&#x7235;&#x58EB;&#x9F13; <pr>ju&#xE9;sh&#xEC;g&#x1D4;</pr></trd>
</drv>
</art>

