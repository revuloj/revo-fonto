<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kombine</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kombine.0o">
  <kap><tld/>o</kap>
  <snc mrk="kombine.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="vest.0o.TEKS">Vesto</ref> unu- a&ubreve; plur-peca
      
      kovranta la korpon tute a&ubreve; plejparte:
      </dif>
      <subsnc mrk="kombine.0o.subvesto">
        <dif>
          Tia tutkorpa <ref tip="super"
          cel="vest.sub0o.TEKS">subvesto</ref>:
          <ekz>
            la norma subvesta&jcirc;o de viroj, virinoj kaj infanoj en la 19a
            jarcento estis la <tld/>o, unupeca trikita tola&jcirc;o, kiu
            kovris
            ne nur la torson, sed anka&ubreve; la brakojn &gcirc;ispojne kaj la
            gambojn
            &gcirc;ismaleole
            <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;subvesto&rightquot;,
            2007-07-10</lok></fnt>.
          </ekz>
        </dif>
        <trd lng="nl">lijfgoed</trd>
      </subsnc>
      <subsnc mrk="kombine.0o.HEJM">
        <dif>
          Unupeca korpovesto por bebo:
          <ekz>
            bebaj <tld/>oj
            <fnt>
              <aut>-</aut>,
              <vrk>rakontoj por infanoj</vrk>,
              <lok><url
              ref="http://www.rakontoj-por-infanoj.com/vestajxoj.html">
              vesta&jcirc;oj</url>, 2006</lok>
            </fnt>.
          </ekz>
        </dif>
        <trdgrp lng="nl">
          <trd>trappelzak</trd>,
          <trd>speelpakje</trd>
        </trdgrp>
      </subsnc>
      <subsnc mrk="kombine.0o.laborvesto">
        <dif>
          Tutkorpa <ref tip="super"
          cel="vest.labor0o.TEKS">laborvesto</ref>
          uzata en malpurigaj a&ubreve; specialaj kondi&ccirc;oj:
          <ekz>
            li revenis de la laboro en sia <tld/>o, malpurigita de
            diversaj
            farboj
            <fnt>
            <aut>L. Karpunina</aut>,
            <vrk>En granda urbo</vrk>,
            <bib>Monato</bib>,
            <lok>jaro 2008a, numero 2a, p. 26a</lok>
            </fnt>.
          </ekz>
        </dif>
        <trdgrp lng="nl">
          <trd>overall</trd>,
          <trd>ruimtepak</trd>,
          <trd>vliegerskostuum</trd>
        </trdgrp>
      </subsnc>
  </snc>
  <trdgrp lng="fr">
    <trd>barboteuse</trd>,
    <trd>combinaison<klr> (v&ecirc;tement)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>combina&ccedil;&atilde;o</trd>,
    <trd>macac&atilde;o</trd>
  </trdgrp>
</drv>
</art>

