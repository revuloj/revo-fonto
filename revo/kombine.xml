<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kombine.xml,v 1.12 2023/12/03 09:58:53 revo Exp $">
<kap>
  <rad>kombine</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kombine.0o">
  <kap><tld/>o</kap>
  <snc mrk="kombine.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="lst" lst="voko:vesta&jcirc;oj" cel="vest.0o.TEKS">Vesto</ref> unu- a&ubreve; plur-peca
      kovranta la korpon tute a&ubreve; plejparte:
    </dif>
    <subsnc mrk="kombine.0o.subvesto">
      <dif>
        Tia tutkorpa <ref tip="super" cel="vest.sub0o.TEKS">subvesto</ref>:
        <ekz>
          la norma subvesta&jcirc;o de viroj, virinoj kaj infanoj en la 19a
          jarcento estis la <tld/>o, unupeca trikita tola&jcirc;o, kiu
          kovris
          ne nur la torson, sed anka&ubreve; la brakojn &gcirc;ispojne kaj la
          gambojn
          &gcirc;ismaleole
          <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;subvesto&rightquot;,
          2007-07-10</lok></fnt>.
        </ekz>
      </dif>
      <trdgrp lng="en">
        <trd>long johns</trd>,
        <trd>long underwear</trd>
      </trdgrp>
      <trd lng="nl">lijfgoed</trd>
    </subsnc>
    <subsnc mrk="kombine.0o.HEJM">
      <dif>
        Unupeca korpovesto por bebo:
        <ekz>
          bebaj <tld/>oj
          <fnt>
            <aut>-</aut>,
            <vrk>rakontoj por infanoj</vrk>,
            <lok><url ref="http://www.rakontoj-por-infanoj.com/vestajxoj.html">
            vesta&jcirc;oj</url>, 2006</lok>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>Strampler</trd>,
        <trd>Strampelanzug</trd>
      </trdgrp>
      <trd lng="en">onesie</trd>
      <trdgrp lng="nl">
        <trd>trappelzak</trd>,
        <trd>speelpakje</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="kombine.0o.laborvesto">
      <dif>
        Tutkorpa <ref tip="super" cel="vest.labor0o.TEKS">laborvesto</ref>
        uzata en malpurigaj a&ubreve; specialaj kondi&ccirc;oj:
        <ekz>
          li revenis de la laboro en sia <tld/>o, malpurigita de
          diversaj
          farboj
          <fnt>
            <aut>L. Karpunina</aut>,
            <vrk>En granda urbo</vrk>,
            <bib>Monato</bib>,
            <lok>jaro 2008a, numero 2a, p. 26a</lok>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="en">
        <trd>coverall</trd>,
        <trd>boilersuit</trd>,
        <trd>jumpsuit</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>overall</trd>,
        <trd>ruimtepak</trd>,
        <trd>vliegerskostuum</trd>
      </trdgrp>
    </subsnc>
  <trdgrp lng="de">
    <trd>Bodysuite</trd>,
    <trd>Jumpsuit</trd>,
    <trd>Fliegerkombi</trd>,
    <trd>Einteiler</trd>,
    <trd>Overall</trd>,
    <trd>Kombi <klr>(Einteiler)</klr></trd>
  </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_m;&c_b;&c_ib;&c_n;&c_a;&c_c;&c_y;&c_ja; <klr tip="amb">(&c_a;&c_d;&c_z;&c_je;&c_n;&c_mol;&c_n;&c_je;)</klr></trd>,
    <trd>&c_k;&c_a;&c_m;&c_b;&c_ib;&c_n;&c_e;&c_z;&c_o;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kombina&ccaron;ka</trd>,
    <trd>kombin&eacute;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>barboteuse</trd>,
    <trd>combinaison<klr> (v&ecirc;tement)</klr></trd>
  </trdgrp>
  <trd lng="pl">kombinezon</trd>
  <trdgrp lng="pt">
    <trd>combina&ccedil;&atilde;o</trd>,
    <trd>macac&atilde;o</trd>
  </trdgrp>
  <trd lng="ro">salopet&abreve;</trd>
  <trd lng="sk">kombin&eacute;za</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_o;&c_m;&c_b;&c_ib;&c_n;&c_a;&c_c;&c_ib;&c_ja; (&c_s;&c_o;&c_r;&c_o;&c_ch;&c_k;&c_a; &c_ib;
    &c_t;&c_r;&c_u;&c_s;&c_i;&c_k;&c_i;)</trd>,
    <trd>&c_k;&c_o;&c_m;&c_b;&c_ib;&c_n;&c_je;&c_n;&c_z;&c_o;&c_n;</trd>
  </trdgrp>
  <trd lng="zh">&#x8FDE;&#x4F53;&#x88E4; <pr>li&#xE1;nt&#x1D0;k&#xF9;</pr></trd>
</drv>
</art>

