<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: balkan.xml,v 1.10 2023/10/29 19:11:00 revo Exp $">
<kap>
  <rad>Balkan</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="balkan.0o">
  <kap><tld/>o,
  <var><kap><tld/>a Montaro</kap></var></kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="balkan.0o.montaro">
    <dif>
      Sudoriente&ubreve;ropa <ref tip="lst" cel="mont.0aro" lst="voko:montaroj">montaro</ref>
      sur teritorio de Bulgarujo kaj Serbujo:
      <ekz>
        la <tld/>a montaro estas la plilongi&gcirc;o de Karpatoj en la suda
        flanko de Danubo, sur la <tld/>a duoninsulo
        <fnt><bib>Viki</bib>, <lok>Balkana montaro</lok></fnt>
      </ekz>
    </dif>
    <trd lng="fr">Grand Balkan</trd>
    <trd lng="pl">Ba&lstroke;kany</trd>
  </snc>
  <snc mrk="balkan.0o.regiono">
    <dif>
      <ref tip="dif" cel="balkani.0o.GEOG">Balkanio</ref>:
      <ekz>
        sub la patroneco de la Pakto pri Stabileco en Sudorienta
        E&ubreve;ropo, ok <tld/>-landoj subskribis interkonsenton pri regiona komerca
        liberigo
        <fnt>
        <aut>E. Georgiev</aut>,
        <vrk>Interkonsento pri komerca liberigo</vrk>,
        <bib>Monato</bib>,
        <lok>2002:2a, p. 13a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">Balkans</trd>
    <trd lng="zh">&#x5DF4;&#x5C14;&#x5E72;&#x534A;&#x5C9B;&#x5404;&#x56FD; <pr>b&#x101;&#x11B;rg&#xE0;nb&#xE0;nd&#x1CE;og&#xE8;gu&#xF3;</pr></trd>
</snc>
  <trd lng="be">&c_B;&c_a;&c_l;&c_k;&c_a;&c_n;&c_y;</trd>
  <trd lng="bg">&c_S;&c_t;&c_a;&c_r;&c_a; &c_p;&c_l;&c_a;&c_n;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>Balk&aacute;n</trd>,
    <trd>Balk&aacute;nsk&eacute; poho&rcaron;&iacute;</trd>
  </trdgrp>
  <trd lng="de">Balkan</trd>
  <trd lng="nl">Balkan</trd>
  <trd lng="no">Balkan</trd>
  <trd lng="pl">P&oacute;&lstroke;wysep Ba&lstroke;ka&nacute;ski</trd>
  <trd lng="ro">Peninsula Balcanic&abreve; </trd>
  <trd lng="sk">Balk&aacute;nske pohorie</trd>
  <trd lng="uk">&c_g;&c_o;&c_r;&c_i;&nbsp;&c_B;&c_a;&c_l;&c_k;&c_a;&c_n;&c_i;</trd>
</drv>
</art>

