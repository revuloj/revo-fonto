<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: marsel.xml,v 1.13 2020/06/27 09:10:55 revo Exp $">
<kap>
  <rad>marseljez</rad>/o
  <fnt><bib>Fab3</bib></fnt>
</kap>
<drv mrk="marsel.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Nacia <ref tip="super" cel="kant.0o.kantajxo">kanto</ref>
      de la francoj:
      <ekz>
        birdo fenikso! &ccirc;u vi &gcirc;in ne konas? &gcirc;i kantis al vi la <tld
        lit="M"/>on, kaj vi
        kantis la plumon, kiu elfalis el &gcirc;iaj flugiloj, &gcirc;i venis en
        paradiza brilo,
        kaj vi eble deturnis vin de &gcirc;i kaj turnis vin al la pasero, kiu
        sidis kun
        orita&jcirc;o sur la flugiloj
        <fnt>
          <bib>Fab3</bib>
          <lok>Birdo Fenikso</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu &ccirc;apitro de &leftquot;<nom>Sternstunden der Menschheit</nom>&rightquot; de
        <nom>Stefan Zweig</nom> temas pri la ekesto
        de <tld lit="M"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=9187"
              >Marseljezo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_s;&c_e;&c_l;&c_mol;&c_je;&c_z;&c_a;</trd>
  <trd lng="ca">Marsellesa</trd>
  <trd lng="de">Marseillaise</trd>
  <trd lng="es">Marsellesa</trd>
  <trd lng="fa">&a_m;&a_A;&a_r;&a_s;&a_kasra;&f_y;&a_kasra;&a_z;</trd>
  <trd lng="fr">Marseillaise</trd>
  <trd lng="hu">Marseillaise</trd>
  <trd lng="nl">Marseillaise</trd>
  <trd lng="pl">Marsylianka<klr> (hymn Francji)</klr></trd>
  <trd lng="pt">Marselhesa</trd>
  <trd lng="ro">La <ind>Marseillaise</ind> <klr>(imnul
  na&#539;ional)</klr></trd>
  <trd lng="ru">&c_M;&c_a;&c_r;&c_s;&c_je;&c_l;&c_mol;&c_je;&c_z;&c_a;</trd>
</drv>
</art>
