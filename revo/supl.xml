<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: supl.xml,v 1.13 2024/03/25 07:28:38 revo Exp $">
<kap>
  <rad>supl</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="supl.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="supl.0a.malrigida">
    <dif>
      <ref tip="dif" cel="rigid.mal0a">Malrigida</ref>:
      <ekz>
        <klr>(ironie)</klr>
        du tukoj tiel <tld/>aj kaj delikataj kiel ledo de
        &scirc;ildo
        <fnt>
          <bib>DonKihxoto</bib>,
          <lok>parto 1a, &ccirc;apitro 16a, p. 111a</lok>
        </fnt>;
      </ekz>
      <ekz>
        li provis denove <tld/>igi siajn kramfajn membrojn
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="&Monato;/2010/009883.php">Trancporto</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        longa <tld/>a kolo
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Ar&gcirc;enta Urbo Amarganto</lok>
        </fnt>;
      </ekz>
      <ekz>
        pri la libro kiel materia produkto: bona blanka papero, klara preso sufi&ccirc;e
        grandlitera, firma bindo en fortika <tld/>a kovrilo
        <fnt>
          <bib>Monato</bib>
          <aut>Christian Declerck</aut>
          <vrk><url ref="&Monato;/2009/009758.php">Panoramo de la Nerona Romo</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiun bandon e&ubreve;femisme li nomis &leftquot;alianco de bonvoluloj&#x201D; &ndash; pli trafe oni devus
        diri &leftquot;alianco de <tld/>uloj&#x201D;
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2007/009202.php">Disfalema alianco</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_n;&c_u;&c_t;&c_k;&c_ib;</trd>,
    <trd>&c_e;&c_l;&c_ja;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ohebn&yacute;</trd>,
    <trd>poddajn&yacute;</trd>,
    <trd>pru&zcaron;n&yacute;</trd>,
    <trd>vl&aacute;&ccaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>beweglich</trd>,
    <trd>flexibel</trd>
  </trdgrp>
  <trd lng="en">supple</trd>
  <trdgrp lng="fr">
    <trd>souple</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hajl&eacute;kony</trd>,
    <trd>rugalmas</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>buigzaam</trd>,
    <trd>flexibel</trd>,
    <trd>elastisch</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>mi&eogonek;kki</trd>,
    <trd>gi&eogonek;tki</trd>,
    <trd>gibki</trd>,
    <trd>elastyczny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_i;&c_b;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_e;&c_l;&c_a;&c_s;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>poddajn&yacute;</trd>,
    <trd>tv&aacute;rliv&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x433;&#x43D;&#x443;&#x447;&#x43A;&#x438;&#x439;</trd>,
    <trd>&#x43C;&#x2019;&#x44F;&#x43A;&#x438;&#x439;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x67D4; <pr>r&#xF3;u</pr></trd>,
    <trd>&#x67D4;&#x9806; <pr>r&#xF3;ush&#xF9;n</pr></trd>,
    <trd>&#x67D4;&#x987A; <pr>r&#xF3;ush&#xF9;n</pr></trd>,
    <trd>&#x67D4;&#x97CC; <pr>r&#xF3;ur&#xE8;n</pr></trd>,
    <trd>&#x67D4;&#x97E7; <pr>r&#xF3;ur&#xE8;n</pr></trd>,
    <trd>&#x53EF;&#x52A8; <pr>k&#x11B;d&#xF2;ng</pr></trd>,
    <trd>&#x53EF;&#x52D5; <pr>k&#x11B;d&#xF2;ng</pr></trd>
  </trdgrp>
</drv>
