<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>supl</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="supl.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <ref tip="dif" cel="rigid.mal0a">malrigida</ref>
    <ekz>
      <klr>(ironie)</klr>
      du tukoj tiel <tld/>aj kaj delikataj kiel ledo de
      &scirc;ildo
      <fnt>
      <bib>DonKihxoto</bib>,
      <lok>parto 1a, &ccirc;apitro 16a, p. 111a</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_n;&c_u;&c_t;&c_k;&c_ib;</trd>,
    <trd>&c_e;&c_l;&c_ja;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>beweglich</trd>,
    <trd>flexibel</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>souple</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hajl&eacute;kony</trd>,
    <trd>rugalmas</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>buigzaam</trd>,
    <trd>flexibel</trd>,
    <trd>elastisch</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_i;&c_b;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_e;&c_l;&c_a;&c_s;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
