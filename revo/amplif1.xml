<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>amplifik</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="amplif1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="amplif1.0i.ELE">
    <uzo tip="fak">ELE</uzo>
    <ref cel="amplif2.0i">amplifi</ref>
    <trd lng="be">&c_u;&c_z;&c_m;&c_a;&c_c;&c_n;&c_ja;&c_c;&c_mol;</trd>
    <trd lng="bg">&c_u;&c_s;&c_i;&c_l;&c_v;&c_a;&c_m;</trd>
    <trd lng="de">verst&auml;rken</trd>
    <trd lng="en">amplify</trd>
    <trd lng="fr">amplifier</trd>
    <trd lng="hu">er&odblac;s&iacute;t<klr tip="ind"> (rezg&eacute;st)</klr></trd>
    <trd lng="nl">versterken</trd>
    <trd lng="ru">&c_u;&c_s;&c_i;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
    <trd lng="sk">amplifikova&tcaron;</trd>
  </snc>
  <trd lng="sk">zosiľovať</trd>
</drv>

<drv mrk="amplif1.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <ref cel="amplif2.0ilo">amplifilo</ref>
    <trd lng="be">&c_u;&c_z;&c_m;&c_a;&c_c;&c_n;&c_ja;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
    <trd lng="bg">&c_u;&c_s;&c_i;&c_l;&c_v;&c_a;&c_t;&c_je;&c_l;</trd>
    <trd lng="de">Verst&auml;rker</trd>
    <trd lng="en">amplifier</trd>
    <trd lng="fr">amplificateur</trd>
    <trd lng="hu">er&odblac;s&iacute;t&odblac;<klr tip="ind"> (rezg&eacute;s-)</klr></trd>
    <trd lng="nl">versterker</trd>
    <trd lng="ru">&c_u;&c_s;&c_i;&c_l;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
  </snc>
  <trd lng="sk">zosilňovač</trd>
</drv>
</art>
