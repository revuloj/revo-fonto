<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Ze&ubreve;s</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="zeuxs.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MIT</uzo>
    <dif>
      Helena supera <ref tip="super" cel="di.0o.politeisma">dio</ref>,
      unue la dio de lumo.
    </dif>
    <ref tip="vid" cel="jupiter.0o.mita">Jupitero</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_D;&c_z;&c_e;&c_w;&c_s;</trd>,
    <trd>&c_Z;&c_je;&c_w;&c_s;</trd>
  </trdgrp>
  <trd lng="ca">Zeus</trd>
  <trd lng="de">Zeus</trd>
  <trd lng="el">&Delta;&jota_acute;&alfa;&sigma_fina;</trd>
  <trd lng="en">Zeus</trd>
  <trd lng="es">Zeus</trd>
  <trd lng="fa">&a_z;&a_y_hamza;&a_w;&a_s;</trd>
  <trd lng="hu">Zeusz</trd>
  <trd lng="pl">Ze&ubreve;us</trd>
  <trd lng="pt">Zeus</trd>
  <trd lng="fr">Zeus</trd>
  <trd lng="ru">&c_Z;&c_je;&c_v;&c_s;</trd>
  <trd lng="sv">Zeus</trd>
  <trd lng="it">Zeus</trd>
  <adm>
    Indas kontroli la grekan tradukon kaj aldoni la klasikan grekan.
    <aut>JF</aut>
  </adm>
</drv>
</art>
