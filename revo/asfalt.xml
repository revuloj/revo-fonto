<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: asfalt.xml,v 1.35 2023/10/28 08:33:09 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>asfalt</rad>/o <fnt><bib>MT</bib></fnt>
</kap>

<drv mrk="asfalt.0o">
  <kap><tld/>o</kap>

  <snc mrk="asfalt.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Nigreca, neflua miksa&jcirc;o esti&gcirc;inta el kerogeno apena&ubreve;
      enhavanta mineralecan substancon:
      <ekz>
        &scirc;i prenis por li keston el kanoj kaj &ccirc;irka&ubreve;&scirc;miris &gcirc;in per
        <tld/>o kaj pe&ccirc;o, kaj
        metis tien la infanon kaj metis &gcirc;in inter la kanojn sur la bordo
        de la Rivero
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 2:3</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="naft.0o">nafto</ref>,
      <ref cel="skist.0o.karbona">skisto<sncref/></ref>
    </refgrp>
  </snc>

  <snc mrk="asfalt.0o.KON">
    <uzo tip="fak">KON</uzo>
    <uzo tip="fak">TRA</uzo>
    <dif>
      <ref tip="vid" cel="bitum.0o">Bitumo</ref> uzata por tegi
      stratojn:
      <ekz>
        &scirc;iaj kalkanumetoj frapetis la asfalton
        <fnt>
          <aut>Konstanten Krysakov</aut>
          <vrk><url ref="&Monato;/2000/006805.php">Kota akvo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 28</lok>
          </fnt>;
      </ekz>
      <ekz>
        dum la rikolta sezono oni sur&scirc;utas grenon sur la varmegan
        <tld/>on por sekigi &gcirc;in
        <fnt>
          <aut>E. Georgiev</aut>,
          <vrk>Nova ponto sur Danubo</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2001a, numero 1a, p. 11a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x67CF;&#x6CB9; <pr>b&#x1CE;iy&#xF3;u</pr></trd>,
    <trd>&#x6CA5;&#x9752; <pr>l&#xEC;q&#x12B;ng</pr></trd>,
    <trd>&#x701D;&#x9751; <pr>l&#xEC;q&#x12B;ng</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_s;&c_f;&c_a;&c_l;&c_mol;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>asfalt</trd>,
    <trd>bitumen</trd>
  </trdgrp>
  <trd lng="de">Asphalt</trd>
  <trd lng="en">asphalt</trd>
  <trd lng="es">asfalto</trd>
  <trd lng="fr">asphalte</trd>
  <trd lng="he">&#x5D0;&#x5B7;&#x5E1;&#x5E4;&#x5B7;&#x5DC;&#x5D8;</trd>
  <trd lng="hu">aszfalt</trd>
  <trd lng="id">aspal</trd>
  <trd lng="it">asfalto</trd>
  <trd lng="ja">&#x30A2;&#x30B9;&#x30D5;&#x30A1;&#x30EB;&#x30C8;</trd>
  <trd lng="nl">asfalt</trd>
  <trd lng="pl">asfalt</trd>
  <trd lng="ru">&c_a;&c_s;&c_f;&c_a;&c_l;&c_mol;&c_t;</trd>
  <trd lng="sk">asfalt</trd>
  <trd lng="sv">asfalt</trd>
  <trd lng="uk">&c_a;&c_s;&c_f;&c_a;&#x43B;&#x44C;&c_t;</trd>
</drv>

<drv mrk="asfalt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="asfalt.0i.KON">
    <uzo tip="fak">KON</uzo>
    <dif>
      Surkovri per <ref cel="asfalt.0o.KON"><tld/>o<sncref/></ref>:
      <ekz>
        la vojoj estis jam relative bone <tld/>itaj sen la sen&ccirc;esaj
        kavoj kaj
        malglata&jcirc;oj ordinare kutimaj en nia lando
        <fnt>
          <bib>Monato</bib>
          <aut>Dmitrij Cibulevskij</aut>
          <vrk><url ref="&Monato;/2007/009112.php">Busvoja&gcirc;o</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        sur la <tld/>ita sena&ubreve;ta parkejo dancas kvino da nigraha&ubreve;taj
        altaj viroj
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2006/008760.php">En fremda lando</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x6CA5;&#x9752;&#x5316; <pr>l&#xEC;q&#x12B;nghu&#xE0;</pr></trd>,
    <trd>&#x701D;&#x9751;&#x5316; <pr>l&#xEC;q&#x12B;nghu&#xE0;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">
  &c_a;&c_s;&c_f;&c_a;&c_l;&c_mol;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">asfaltovat</trd>
  <trd lng="de">asphaltieren</trd>
  <trd lng="en">lay <ind>asphalt</ind></trd>
  <trd lng="es">asfaltar</trd>
  <trd lng="fr">asphalter</trd>
  <trd lng="he">&#x5DC;&#x5E9;&#x5D9;&#x5DD; &#x5D0;&#x5B7;&#x5E1;&#x5E4;&#x5B7;&#x5DC;&#x5D8;</trd>
  <trd lng="hu">aszfaltoz</trd>
  <trd lng="id">meng<ind>aspal</ind></trd>
  <trd lng="it">asfaltare</trd>
  <trd lng="ja">&#x30A2;&#x30B9;&#x30D5;&#x30A1;&#x30EB;&#x30C8;&#x3067;&#x8217;&#x88C5;&#x3059;&#x308B;  <pr>&#x30A2;&#x30B9;&#x30D5;&#x30A1;&#x30EB;&#x30C8;&#x3067;&#x307B;&#x305D;&#x3046;&#x3059;&#x308B;</pr></trd>
  <trd lng="nl">asfalteren</trd>
  <trd lng="pl">asfaltowa&cacute;</trd>
  <trd lng="ru">&c_a;&c_s;&c_f;&c_a;&c_l;&c_mol;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">asfaltova&tcaron;</trd>
  <trd lng="sv">asfaltera</trd>
  <trd lng="uk">&c_a;&c_s;&c_f;&c_a;&#x43B;&#x44C;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>
</art>
