<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>asfalt</rad>/o
</kap>
<drv mrk="asfalt.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Speco de bitumo, kiu servas por surkovri stratojn.
    </dif>
  </snc>
  <trd lng="fr">asphalte</trd>
  <trd lng="nl">asfalt</trd>
  <trd lng="ru">&c_a;&c_s;&c_f;&c_a;&c_l;&c_mol;&c_t;</trd>
  <trd lng="sv">asfalt</trd>
</drv>

<drv mrk="asfalt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Surkovri per <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">asphalter</trd>
  <trd lng="nl">asfalteren</trd>
  <trd lng="ru">&c_a;&c_s;&c_f;&c_a;&c_l;&c_mol;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">asfaltera</trd>
</drv>
</art>
