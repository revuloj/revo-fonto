<kap>
  <rad>Rumani</rad>/o
</kap>

<drv mrk="rumani.R0o">
  <kap><tld/>o</kap>
  <snc mrk="rumani.R0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="ruman.R0ujo.GEOG">Rumanujo</ref>:
      <ekz>
        komence de junio en <tld/>o estis pli ol unu miliono da senlaboruloj, kio egalas
        al 9,3 centonoj de la rumana lo&gcirc;antaro
        <fnt>
          
          <vrk><url ref="&Monato;/1993/A930902.php">Noticoj</url></vrk>
          <bib>Monato</bib>
          <lok>1993/09, p. 4</lok>
        </fnt>;
      </ekz>
      <ekz>
        mencii la situacion de hungaroj en <tld/>o
        <fnt>
          <aut>Sylvia Brylska</aut>
          <vrk><url ref="&Monato;/1993/A930801.php">&Ccirc;u reiri al naciaj
          radikoj?</url></vrk>
          <bib>Monato</bib>
          <lok>1993/08, p. 12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_R;&c_u;&c_m;&c_y;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_R;&c_u;&c_m;&c_malmol;&c_n;&c_i;&c_ja;</trd>
  <trd lng="de">Rum&auml;nien</trd>
  <trd lng="en">Romania</trd>
  <trd lng="fr">Roumanie</trd>
  <trd lng="id">Rumania</trd>
  <trd lng="ja">&#x30EB;&#x30FC;&#x30DE;&#x30CB;&#x30A2;</trd>
  <trd lng="nl">Roemeni&euml;</trd>
  <trd lng="pl">Rumunia</trd>
  <trd lng="uk">&c_R;&c_u;&#x43C;&c_u;&#x43D;&c_ib;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x7F57;&#x9A6C;&#x5C3C;&#x4E9A; <pr>Lu&#xF3;m&#x1CE;n&#xED;y&#xE0;</pr></trd>,
    <trd>&#x7F85;&#x99AC;&#x5C3C;&#x4E9E; <pr>Lu&#xF3;m&#x1CE;n&#xED;y&#xE0;</pr></trd>
  </trdgrp>
