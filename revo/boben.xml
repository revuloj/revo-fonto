<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>boben</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="boben.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="boben.0o.KOMUNE">
    <dif>
      Cilindroforma ilo sur kiu oni &ccirc;irka&ubreve;volvas fadenon.
    </dif>
    <trdgrp lng="de">
      <trd>Spule</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>ors&oacute;<klr tip="ind"> (cs&eacute;ve)</klr></trd>,
      <trd>cs&eacute;ve</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>spool</trd>,
      <trd>bobbin</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>szpula</trd>,
      <trd>szpulka</trd>
    </trdgrp>
    <trd lng="pt">bobina</trd>
  </snc>
  <snc mrk="boben.0o.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Speciala aran&gcirc;o konsistanta el cilindro
      &ccirc;irka&ubreve;volvita per elektra fadeno.
    </dif>
    <trdgrp lng="en">
      <trd>inductor</trd>,
      <trd>coil</trd>,
      <trd>choke</trd>,
      <trd>choke coil</trd>,
      <trd>reactor</trd>
    </trdgrp>
    <trd lng="hu">tekercs</trd>
    <trd lng="pl">cewka</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_p;&c_u;&c_l;&c_mol;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_p;&c_u;&c_l;&c_ja;</trd>
    <trd>&c_b;&c_a;&c_b;&c_ib;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="br">kanell</trd>
  <trd lng="cs">c&iacute;vka s vinut&iacute;m</trd>
  <trd lng="es">bobina</trd>
  <trdgrp lng="fr">
    <trd>bobine</trd>,
    <trd>fusette</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_t;&c_u;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_b;&c_o;&c_b;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">cievka s vinut&iacute;m</trd>
</drv>

<drv mrk="boben.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="boben.0ego.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Granda <tld/>o por &ccirc;irka&ubreve;volvi &scirc;nuregon
      &ccirc;irka&ubreve; &gcirc;in.
    </dif>
    <refgrp tip="vid">
      <ref cel="kapsta.0o">kapstano</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n; <klr>(&c_n;&c_a;&c_p;&c_r;. &c_p;&c_a;&c_d;'&c_jo;&c_m;&c_n;&c_a;&c_g;&c_a; &c_k;&c_r;&c_a;&c_n;&c_a;)</klr></trd>
  <trd lng="cs">buben <klr>(na kabel)</klr></trd>
  <trdgrp lng="en">
    <trd>spool</trd>,
    <trd>reel</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>tambour enrouleur</trd>,
    <trd>touret<klr tip="ind"> (d&eacute;vidoir)</klr></trd>
  </trdgrp>
  <trd lng="hu">k&aacute;beldob</trd>
  <trd lng="pl">b&eogonek;ben<klr> nawojowy</klr></trd>
  <trd lng="ru">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n; <klr>(&c_p;&c_o;&c_d;&c_malmol;&c_jo;&c_m;&c_n;&c_o;&c_g;&c_o; &c_k;&c_r;&c_a;&c_n;&c_a;, &c_l;&c_je;&c_b;&c_jo;&c_d;&c_k;&c_i;)</klr></trd>
  <trd lng="sk">bubon <klr>(na k&aacute;bel)</klr></trd>
</drv>

<drv mrk="boben.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="boben.0i.TEK">
    <dif>
      Volvi &ccirc;irka&ubreve; <tld/>o.
    </dif>
    <refgrp tip="vid">
      <ref cel="vind.0i">vindi</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_m;&c_o;&c_t;&c_v;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_n;&c_a; &c_sh;&c_p;&c_u;&c_l;&c_ju;,
  &c_b;&c_a;&c_b;&c_ib;&c_n;&c_u;)</klr></trd>
  <trd lng="br">puna&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>motat</trd>,
    <trd>nav&iacute;jet na c&iacute;vku</trd>,
    <trd>soukat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>aufspulen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>wind</trd>,
    <trd>spool</trd>
  </trdgrp>
  <trd lng="fr">bobiner</trd>
  <trdgrp lng="hu">
    <trd>felcs&eacute;v&eacute;l</trd>,
    <trd>feltekercsel</trd>
  </trdgrp>
  <trd lng="pl">nawija&cacute;<klr> na szpulk&eogonek;, na b&eogonek;ben, cewk&eogonek;</klr></trd>
  <trd lng="ru">&c_n;&c_a;&c_m;&c_a;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;
  <klr>(&c_n;&c_a; &c_k;&c_a;&c_t;&c_u;&c_sh;&c_k;&c_u;)</klr></trd>
  <trdgrp lng="sk">
    <trd>mota&tcaron;</trd>,
    <trd>nato&ccaron;i&tcaron; na cievku</trd>
  </trdgrp>
</drv>

<drv mrk="boben.glit0o">
  <kap>glit<tld/>o</kap>
  <snc mrk="boben.glit0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <ref tip="dif" cel="naved.0o">navedo</ref>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_w;&c_n;&c_o;&c_k; <klr tip="amb">(&c_t;&c_k;&c_a;&c_c;&c_k;&c_ib;)</klr></trd>
  <trd lng="br">bulzun</trd>
  <trd lng="cs">&ccaron;lunek</trd>
  <trd lng="en">shuttle</trd>
  <trd lng="fr">navette<klr> (tissage)</klr></trd>
  <trd lng="hu">vet&eacute;l&odblac;</trd>
  <trd lng="pl">cz&oacute;&lstroke;enko<klr> tkackie</klr></trd>
  <trd lng="ru">&c_t;&c_k;&c_a;&c_c;&c_k;&c_i;&c_j; &c_ch;&c_je;&c_l;&c_n;&c_o;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>&ccaron;lnok <klr>(v &scaron;ijacom stroji, tk&aacute;&ccaron;skom stroji)</klr></trd>,
    <trd>&ccaron;lnok <klr>(v &scaron;ijacom stroji| tk&aacute;&ccaron;skom stroji)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="boben.lud0o">
  <kap>lud<tld/>o</kap>
  <snc mrk="boben.lud0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="lst" cel="lud.0ilo.SPO" lst="voko:ludiloj">Ludilo</ref>
      konsistanta el pezeta cilindro pendanta de &scirc;nuro,
      kiu pro skuo alterne volvi&gcirc;as kaj malvolvi&gcirc;as,
      tirante la cilindron supren kaj malsupren:
      <ekz>
        Karal plu pa&scirc;is en la &ccirc;ambro tien kaj reen, tien 
        kaj reen, lerte funkciigante lud<tld/>on
        <fnt><aut>C. Piron</aut>, <vrk>&Ccirc;u li 
        venis trakosme?</vrk>, <lok>1980</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">yoyo</trd>
</drv>
</art>

