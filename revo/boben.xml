<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>boben</rad>/o
</kap>
<drv mrk="boben.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Cilindroforma ilo sur kiu oni &ccirc;irka&ubreve;volvas fadenon.
    </dif>
    <trdgrp lng="en">
      <trd>spool</trd>,
      <trd>bobbin</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">ELE</uzo>
    <dif>
      Speciala aran&gcirc;o konsistanta el cilindro &ccirc;irka&ubreve;volvita per elektra
      fadeno.
    </dif>
    <trdgrp lng="en">
      <trd>inductor</trd>,
      <trd>coil</trd>,
      <trd>choke</trd>,
      <trd>choke coil</trd>,
      <trd>reactor</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>bobine</trd>,
    <trd>fusette</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_t;&c_u;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_b;&c_o;&c_b;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="boben.0ego">
  <kap><tld/>ego</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Granda <tld/>o por &ccirc;irka&ubreve;volvi &scirc;nuregon &ccirc;irka&ubreve; &gcirc;in.
    </dif>
    <refgrp tip="vid">
      <ref cel="kapsta.0o">kapstano</ref>.
    </refgrp>
    <trdgrp lng="en">
      <trd>spool</trd>,
      <trd>reel</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>tambour enrouleur</trd>,
      <trd>touret<klr tip="ind"> (d&eacute;vidoir)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="ru">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n; <klr>(&c_p;&c_o;&c_d;&c_malmol;&c_jo;&c_m;&c_n;&c_o;&c_g;&c_o; &c_k;&c_r;&c_a;&c_n;&c_a;,
  &c_l;&c_je;&c_b;&c_jo;&c_d;&c_k;&c_i;)</klr></trd>
</drv>

<drv mrk="boben.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Volvi &ccirc;irka&ubreve; <tld/>o.
    </dif>
    <refgrp tip="vid">
      <ref cel="vind.0i">vindi</ref>.
    </refgrp>
    <trdgrp lng="en">
      <trd>wind</trd>,
      <trd>spool</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">bobiner</trd>
  <trd lng="ru">&c_n;&c_a;&c_m;&c_a;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_n;&c_a; &c_k;&c_a;&c_t;&c_u;&c_sh;&c_k;&c_u;)</klr></trd>
</drv>

<drv mrk="boben.glit0o">
  <kap>glit<tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <ref tip="dif" cel="naved.0o">navedo.</ref>
    <trd lng="en">shuttle</trd>
  </snc>
  <trd lng="ru">&c_t;&c_k;&c_a;&c_c;&c_k;&c_i;&c_j; &c_ch;&c_je;&c_l;&c_n;&c_o;&c_k;</trd>
</drv>
</art>
