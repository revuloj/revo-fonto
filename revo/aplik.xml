<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>aplik</rad>/i
</kap>
<drv mrk="aplik.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Uzi por praktiko teoriajn sciojn:
      <ekz>
        <tld/>i metodon, regulon,
        le&gcirc;on;
      </ekz>
      <ekz>
        <tld/>i sian sperton, talenton<fnt>B</fnt>
      </ekz>
      <ekz>
        al la solvo de la sociaj problemoj;
      </ekz>
      <ekz>
        kion utilas konsilo ne <tld/>ata?
      </ekz>
      <ekz>
        tiu rimarko <tld/>i&gcirc;as al la nuna okazo;
      </ekz>
      <ekz>
        puraj kaj <tld/>itaj sciencoj.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_a;&c_s;&c_t;&c_a;&c_s;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>, 
      <trd>&c_d;&c_a;&c_p;&c_a;&c_s;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>, 
      <trd>&c_p;&c_r;&c_y;&c_k;&c_l;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>, 
      <trd>&c_p;&c_r;&c_y;&c_m;&c_ja;&c_n;&c_ja;&c_c;&c_mol;</trd> 
    </trdgrp>  
  </snc>
  <snc>
    <dif>
      Uzi por speciala celo, en speciala okazo:
      <ekz>
        <tld/>i kuracilon<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i severajn rimedojn;
      </ekz>
      <ekz>
        <tld/>i al iu la faman ekkrion de la poeto<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="util.0igi">utiligi</ref>,
      <ref cel="prakti.0i">praktiki</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_u;&c_zh;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>, 
      <trd>&c_k;&c_a;&c_r;&c_y;&c_s;&c_t;&c_a;&c_c;&c_mol;</trd> 
    </trdgrp>  
  </snc>
  <trd lng="de">anwenden</trd>
  <trd lng="en">apply</trd>
  <trd lng="es">aplicar</trd>
  <trd lng="fr">appliquer</trd>
  <trd lng="it">applicare</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_l;&c_a;&c_g;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_m;&c_je;&c_n;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_s;&c_p;&c_o;&c_l;&c_mol;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">aplikova&tcaron;</trd>
</drv>
</art>
