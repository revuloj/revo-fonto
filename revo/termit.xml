<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: termit.xml,v 1.20 2024/05/10 08:31:31 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>termit</rad>/oj
</kap>
<drv mrk="termit.0oj">
  <kap><tld/>oj</kap>
    <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_ordoj" cel="ord.0o.BIO">Ordo</ref>
      el la klaso <ref tip="super" cel="insekt.0oj">insektoj</ref>
      (<trd lng="la">Isoptera</trd>), el tropikaj landoj, vivanta socie
      kiel la formikoj kaj konstruanta altajn, fortikajn lo&gcirc;ejojn:
      <ekz>
        la nesto de la <tld/>oj havas la altecon de almena&ubreve; kvar metroj kaj tiuj &ccirc;i
        insektoj mem la altecon de ne pli ol duono da centimetro, la lo&gcirc;ejo sekve
        estas okcent fojojn pli alta, ol &gcirc;iaj konstruantoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        Kaj sur la belon &ccirc;i <tld/>oj nigraj / Kakas, kaj &ccirc;irka&ubreve; miron volvas muron / De
        kotobrikoj
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2009/009682.php">La Pendantaj &Gcirc;ardenoj estis son&gcirc;o</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/2b/Nest_of_Formosan_subterranean_termites_(Coptotermes_formosanus).jpg/377px-Nest_of_Formosan_subterranean_termites_(Coptotermes_formosanus).jpg" prm="PD"><tld/>oj &ccirc;e sia nesto (Coptotermes formosanus)
        <fnt>
          <aut>Scott Bauer</aut>
          <lok><url ref="&WCW;/File:Nest_of_Formosan_subterranean_termites_(Coptotermes_formosanus).jpg">&WCW;/File:Nest_of_Formosan_subterranean_termites_(Coptotermes_formosanus).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_r;&c_m;&c_ib;&c_t;&c_y; <klr tip="amb">(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="bg">&c_t;&c_je;&c_r;&c_m;&c_i;&c_t;&c_i;</trd>
  <trdgrp lng="ca">
    <trd>is&ograve;pters</trd>,
    <trd>t&egrave;rmits</trd>
  </trdgrp>
  <trd lng="de">Termiten</trd>
  <trd lng="en">termite</trd>
  <trd lng="fr">termites</trd>
  <trd lng="hu">termeszek</trd>
  <trd lng="nl">termieten</trd>
  <trd lng="pl">termity</trd>
  <trdgrp lng="pt">
    <trd>is&oacute;pteros</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_r;&c_m;&c_i;&c_t;&c_y; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trdgrp lng="zh">
    <trd>&#x767D;&#x8681; <pr>b&#xE1;iy&#x1D0;</pr></trd>,
    <trd>&#x767D;&#x87FB; <pr>b&#xE1;iy&#x1D0;</pr></trd>,
    <trd>&#x86C0; <pr>zh&#xF9;</pr></trd>
  </trdgrp>
</drv>
</art>
