  <trd lng="de">Termiten</trd>
  <trd lng="nl">termieten</trd>
  <trd lng="ru">&c_t;&c_je;&c_r;&c_m;&c_i;&c_t;&c_y; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
