<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>termit</rad>/oj
</kap>
<drv mrk="termit.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="insekt.0oj">insektoj</ref>
      (<trd lng="la">Isoptera</trd>), el tropikaj landoj, vivanta socie
      kiel la formikoj kaj konstruanta altajn, fortikajn lo&gcirc;ejojn.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_r;&c_m;&c_ib;&c_t;&c_y; <klr tip="amb">(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="bg">&c_t;&c_je;&c_r;&c_m;&c_i;&c_t;&c_i;</trd>
  <trd lng="de">Termiten</trd>
  <trd lng="en">termite</trd>
  <trd lng="hu">termeszek</trd>
  <trd lng="nl">termieten</trd>
  <trd lng="pl">termity</trd>
  <trdgrp lng="pt">
    <trd>is&oacute;pteros</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_r;&c_m;&c_i;&c_t;&c_y; <klr
  tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
</drv>
</art>
