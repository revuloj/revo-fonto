      <trd>Cupido</trd>,
      <trd>Eros</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_A;&c_m;&c_u;&c_r;</trd>,
      <trd>&c_K;&c_u;&c_p;&c_i;&c_d;&c_o;&c_n;</trd>
    </trdgrp>
    <trd lng="sk">Amor</trd>
  </snc>
