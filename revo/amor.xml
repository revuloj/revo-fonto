      <trd>Amor</trd>,
      <trd>Eros</trd>,
      <trd>Gott der <ind>Liebe</ind></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>Amour</trd>,
      <trd>Cupid</trd>,
      <trd>Eros</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>Amor</trd>,
      <trd>Cupido</trd>,
      <trd>Eros</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>Amour<klr tip="ind"> (mythologie)</klr></trd>,
      <trd>Cupidon</trd>,
      <trd>&Eacute;ros</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&Aacute;mor</trd>,
      <trd>Cupido</trd>,
      <trd>Er&oacute;sz</trd>
    </trdgrp>
    <trdgrp lng="nl">
