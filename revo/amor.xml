<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>amor</rad>/o
</kap>
<drv mrk="amor.A0o">
  <kap><tld lit="A"/>o</kap>
  <snc>
    <uzo tip="fak">MIT</uzo>
    <dif>
      Dio de amo en antikvaj Grekujo, Romio kaj Hindujo
      prezentata per figuro de nuda, flugilhava knabo
      ark-pafanta al enami&gcirc;ontoj.
    </dif>
    <trdgrp lng="de">
      <trd>Amor</trd>,
      <trd>Eros</trd>,
      <trd>Gott der <ind>Liebe</ind></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>Amour</trd>,
      <trd>Cupid</trd>,
      <trd>Eros</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>Amor</trd>,
      <trd>Cupido</trd>,
      <trd>Eros</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>Amour<klr> (mythologie)</klr></trd>,
      <trd>Cupidon</trd>,
      <trd>&Eacute;ros</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&Aacute;mor</trd>,
      <trd>Cupido</trd>,
      <trd>Er&oacute;sz</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>Amor</trd>,
      <trd>Cupido</trd>,
      <trd>Eros</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_A;&c_m;&c_u;&c_r;</trd>,
      <trd>&c_K;&c_u;&c_p;&c_i;&c_d;&c_o;&c_n;</trd>
    </trdgrp>
    <trd lng="sk">Amor</trd>
  </snc>
</drv>

<drv mrk="amor.0o">
  <kap><tld/>o</kap>
  <snc mrk="amor.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Figuro de nuda, flugilhava bubeto
      uzata orname en E&ubreve;ropa klasika
      pentrarto plejofte monument-orname.
    </dif>
    <trdgrp lng="en">
      <trd>Kupido</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>amour<klr> (beaux-arts)</klr></trd>,
      <trd>putto</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>amorett</trd>,
      <trd>putt&oacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_a;&c_m;&c_u;&c_r;</trd>,
      <trd>&c_k;&c_u;&c_p;&c_i;&c_d;&c_o;&c_n;</trd>
    </trdgrp>
  </snc>
  <snc mrk="amor.0o.seksa">
    <dif>
      Seksa pasio kaj volupta inklino al iu:
      <ekz>
        la noveloj [...] prezentas diversajn implikojn, embarasojn,
        ruzojn, sekvojn kaj entute cirkonstancojn de <tld/>o inter virino
        kaj viro<fnt><aut>Sten Johansson</aut>: <vrk>Tri tagoj da &gcirc;uo
        (Giovanni Boccaccio, Dekamerono, unuaj tri tagoj)</vrk>,
        <bib>Monato</bib><lok>jaro 1996a, numero 12a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>k&ouml;rperliche <ind>Liebe</ind></trd>,
      <trd>Geilheit</trd>,
      <trd>erotische <ind>Zuneigung</ind></trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>amor (f&iacute;sico)</trd>,
      <trd>relaci&oacute;n sexual</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>amour (physique)</trd>,
      <trd>relation sexuelle</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>testi szerelem</trd>,
      <trd>szex</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>het <ind>vrijen</ind></trd>,
      <trd>sexuele relatie</trd>,
      <trd>lichamelijke <ind>liefde</ind></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_l;&c_ju;&c_b;&c_o;&c_v;&c_n;&c_a;&c_ja;
      &c_s;&c_t;&c_r;&c_a;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_l;&c_ju;&c_b;&c_o;&c_v;&c_mol;
      <klr>(&c_f;&c_i;&c_z;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja;)</klr></trd>,
      <trd>&c_s;&c_je;&c_k;&c_s;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="amor.0i">
  <kap><tld/>i</kap>
  <snc>
    <dif>
      Sekse ami:
      <ekz>
        mi vere ne scias, &ccirc;u ili <tld/>is dum la tuta
        nokto<fnt><aut>Alberto Garc&iacute;a Fumero</aut>: <vrk>Kura&gcirc;o</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 2a, p.
        23-25a</lok></fnt>.
      </ekz> Kp. <ref tip="sin" cel="fik.0i">fiki</ref>, <ref tip="sin" cel="koit.0i">koiti</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
      <trd><klr>(</klr>sexuell) <ind>lieben</ind></trd>,
      <trd>erotisch <ind>lieben</ind></trd>,
      <trd>Liebe <ind>machen</ind></trd>,
      <trd>Sex machen</trd>,
      <trd>Sex haben</trd>
  </trdgrp>
  <trd lng="en">make love</trd>
  <trd lng="es">hacer el amor</trd>
  <trdgrp lng="fr">
    <trd>faire l'<ind>amour</ind></trd>,
    <trd>coucher (avec)</trd>
  </trdgrp>
  <trd lng="hu">szexel</trd>
  <trd lng="it">fare l'<ind>amore</ind></trd>
  <trdgrp lng="nl">
    <trd>vrijen</trd>,
    <trd>neuken</trd>,
    <trd>liefde bedrijven</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;&c_s;&c_ja;
  &c_l;&c_ju;&c_b;&c_o;&c_v;&c_mol;&c_ju;</trd>
  <trd lng="sk">milova&tcaron;</trd>
</drv>

<drv mrk="amor.0anto">
  <kap><tld/>ant(in)o</kap>
  <snc>
    <dif>
      Kiu sekse amas.
    </dif>
  </snc>
  <trdgrp lng="de">
      <trd>Liebhaber</trd>,
      <trd>Gespiele</trd>,
      <trd>Gespielin</trd>
  </trdgrp>
  <trd lng="en">lover</trd>
  <trd lng="es">amante</trd>
  <trdgrp lng="fr">
    <trd>amant(e)</trd>,
    <trd>ma&icirc;tresse</trd>,
    <trd>petit(e) <ind>ami(e)</ind></trd>
  </trdgrp>
  <trd lng="hu">szeret&odblac;<klr tip="amb"> (testi)</klr></trd>
  <trdgrp lng="nl">
    <trd>lief</trd>,
    <trd>bijslaap</trd>
  </trdgrp>
  <trd lng="it">amante</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_ju;&c_b;&c_o;&c_v;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_l;&c_ju;&c_b;&c_o;&c_v;&c_n;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="amor.0isto">
  <kap><tld/>ist(in)o</kap>
  <snc>
    <dif>
      Kiu profesie sekse agas.
    </dif>
  </snc>
  <trdgrp lng="de">
      <trd>Prostituierte(r)</trd>,
      <trd>Hure</trd>,
      <trd>Nutte</trd>,
      <trd>Callboy</trd>,
      <trd>Callgirl</trd>,
      <trd>Kurtisane<klr> [ark]</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>prostitute</trd>,
    <trd>whore</trd>,
    <trd>hooker</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gigolo</trd>,
    <trd>prostitu&eacute;(e)</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>selyemfi&uacute;</trd>,
    <trd>zsigol&oacute;</trd>,
    <trd>kitartott</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gigolo</trd>,
    <trd>prostituee</trd>,
    <trd>hoer</trd>
  </trdgrp>
</drv>
</art>
