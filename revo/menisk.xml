<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: menisk.xml,v 1.7 2023/09/02 06:04:11 revo Exp $">
<kap>
  <rad>menisk</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="menisk.0o">
  <kap><tld/>o</kap>
  <snc mrk="menisk.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Malgranda kava kartilago-disketo,
      kiu faciligas la intergliton de du
      ostoj &ccirc;e artiko:
      <ekz>
        <tld/>o estas gravega peco de kartilago ene de genuo
        <fnt>
          <aut>B. Wennergren</aut>,
          <vrk><url ref="http://de.bertilow.com/blogo/2007/10/lambastonoj/">
          Lambastonoj</url></vrk>,
          <lok>BBBlogo, 2007-10-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_e;&c_n;&c_ib;&c_s;&c_k;</trd>,
    <trd>&c_s;&c_ja;&c_r;&c_p;&c_o;&c_k; <klr>(&c_s;&c_u;&c_s;&c_t;&c_a;&c_w;&c_n;&c_y;)</klr></trd>
  </trdgrp>
  <trd lng="cs">vypuklodut&aacute; &ccaron;o&ccaron;ka</trd>
  <trd lng="en">meniscus (cartilage)</trd>
  <trd lng="fr">m&eacute;nisque</trd>
  <trdgrp lng="ja">
    <trd>&#x30E1;&#x30CB;&#x30B9;&#x30AB;&#x30B9;</trd>,
    <trd>&#x30E1;&#x30CB;&#x30B9;&#x30AB;&#x30B9;&#x30EC;&#x30F3;&#x30BA;</trd>
  </trdgrp>
  <trd lng="sk">vypuklo dut&aacute; &scaron;o&scaron;ovka</trd>
  <trd lng="uk">&c_m;&c_je;&c_n;&c_ib;&c_s;&c_k;</trd>
</drv>
</art>

