<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: loren.xml,v 1.5 2023/09/02 05:33:11 revo Exp $">
<kap>
    <rad>Loren</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="loren.0o">
  <kap><tld/>o</kap>
  <snc mrk="loren.0o.regiono">
    <uzo tip="fak">GEOG</uzo>
    <dif>
       <ref tip="lst" cel="region.0o" lst="voko:regionoj">Regiono</ref>
       en la nordoriento de <ref tip="malprt" cel="franc.F0ujo">Francujo</ref>:
      <ekz>
        <tld/>o <klr>[...]</klr> apudas al Alzaco, <klr>[...]</klr>
        Belgio, 
        Luksemburgo, kaj Germanio
        <fnt><bib>Viki</bib>, <lok>Loreno</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_L;&c_ja;&c_t;&c_a;&c_r;&c_y;&c_n;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="de">Lothringen</trd>
  <trd lng="fr">Lorraine</trd>
  <trd lng="ja">&#x30ED;&#x30EC;&#x30FC;&#x30CC;&#x5730;&#x65B9;  [<ind>&#x30ED;&#x30EC;&#x30FC;&#x30CC;&#x3061;&#x307B;&#x3046;</ind>]</trd>
  <trd lng="pl">Lotaryngia</trd>     
  <trd lng="ro">Lorena</trd>
  <trd lng="uk">&#x41B;&#x43E;&c_t;&c_a;&c_r;&c_ib;&#x43D;&c_gu;&c_ib;&#x44F;</trd>
</drv>
</art>

