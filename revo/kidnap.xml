<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kidnap.xml,v 1.10 2022/03/21 17:51:57 revo Exp $">
<kap>
  <rad>kidnap</rad>/i <fnt><bib>PIV2</bib></fnt>
</kap>

<drv mrk="kidnap.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="kidnap.0i.JUR">
    <dif>
      <ref tip="super" cel="kapt.for0i.KOMUNE">Forkapti</ref> iun,
      ekzemple por el&ccirc;anta&gcirc;i monon:
      <ekz>
        bando da krimuloj <tld/>is junan, konati&gcirc;antan artiston
        <fnt>
        <aut>Don Harlow</aut>,
        <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Recenzoj/sercxu.html">[recenzo
        de] Ser&ccirc;u, kaj vi trovos!</url> / Nemere Istv&aacute;n. - 1999.</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ostagx.0igi.POL">osta&gcirc;igi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_k;&c_r;&c_a;&c_d;&c_a;&c_c;&c_mol; <klr>(&c_z; &c_m;&c_e;&c_t;&c_a;&c_j; &c_v;&c_y;&c_k;&c_u;&c_p;&c_u;)</klr></trd>
  <trdgrp lng="ca">
    <trd>segrestar</trd>,
    <trd>raptar</trd>
  </trdgrp>
  <trd lng="cs">un&eacute;st <klr>(&ccaron;lov&ecaron;ka)</klr></trd>
  <trdgrp lng="de">
    <trd>entf&uuml;hren</trd>,
    <trd>kidnappen</trd>
  </trdgrp>
  <trd lng="fr">kidnapper</trd>
  <trd lng="hu">rabol <klr tip="ind">(embert)</klr></trd>
  <trdgrp lng="nl">
    <trd>ontvoeren</trd>,
    <trd>kidnappen</trd>
  </trdgrp>
  <trd lng="pl">porywa&cacute;</trd>
  <trdgrp lng="ro">
    <trd>apuca</trd>,
    <trd>ascunde</trd>
  </trdgrp>
  <trd lng="sk">unies&tcaron; <klr>(&ccaron;loveka)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x432;&#x438;&#x43A;&#x440;&#x430;&#x434;&#x430;&#x442;&#x438; &#x43B;&#x44E;&#x434;&#x435;&#x439; (&#x437; &#x43C;&#x435;&#x442;&#x43E;&#x44E; &#x448;&#x430;&#x43D;&#x442;&#x430;&#x436;&#x443;</trd>,
    <trd>&#x432;&#x438;&#x43A;&#x443;&#x43F;&#x443; &#x456; &#x442;.&#x456;&#x43D;.)</trd>
  </trdgrp>
</drv>

<drv mrk="kidnap.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="kidnap.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Kontra&ubreve;vola forkapto de homo:
      <ekz>
        kio, do, se Nolan iam liberi&gcirc;us kaj procesus pri <klr>...</klr>
        <tld/>o kontra&ubreve; &ccirc;iu homo, kiu iam prizorgis lin
        <fnt>
        <aut>Edward Everett Hale, trad. E. L. Robinson, trad. Don
        Harlow</aut>,
        <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Noveloj/La_Viro_Senpatria.html">La
        viro senpatria</url></vrk>,
        <lok>1907 kaj 1994</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_d;&c_n;&c_e;&c_p;&c_ib;&c_n;&c_g;</trd>,
    <trd>&c_v;&c_y;&c_k;&c_r;&c_a;&c_d;&c_a;&c_n;&c_mol;&c_n;&c_je; <klr>(&c_z; &c_m;&c_e;&c_t;&c_a;&c_j; &c_v;&c_y;&c_k;&c_u;&c_p;&c_u;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>segrest</trd>,
    <trd>rapte</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Entf&uuml;hrung</trd>,
    <trd>Kidnappen</trd>
  </trdgrp>
  <trd lng="fr">kidnapping</trd>
  <trd lng="hu">emberrabl&aacute;s</trd>
  <trdgrp lng="nl">
    <trd>ontvoering</trd>,
    <trd>kidnapping</trd>
  </trdgrp>
  <trd lng="pl">porwanie <klr>(przest&eogonek;pstwo)</klr></trd>     
  <trd lng="ro">r&abreve;pire</trd>
</drv>
</art>

