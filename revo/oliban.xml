<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: oliban.xml,v 1.20 2024/01/30 16:09:26 revo Exp $">
<kap>
  <rad>oliban</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="oliban.0o">
  <kap><tld/>o</kap>
  <snc mrk="oliban.0o.PAR">
    <dif>
      Tre hela <ref tip="super" cel="rezin.0o">rezino</ref> 
      eltirata el orientafrika-araba arbo (<nom>Boswellia sacra</nom>) kaj uzata kiel 
      <ref tip="super" cel="incens.0o.rezino">incenso</ref> a&ubreve;
      medikamentero:
      <ekz>
        fumo parfumita per <ref cel="mirh.0o">mirho</ref> kaj <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 3:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        prenu al vi aroma&jcirc;on: <ref cel="balzam.0o.PAR">balzamo</ref>n kaj
        <ref cel="stakt.0o">stakto</ref>n kaj <ref cel="galban.0o">galbano</ref>n
        bonodoran kaj puran <tld/>on, po egalaj partoj
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 30:34</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o ja ne povas tiom &ccirc;i odora&ccirc;i
        <fnt>
          <aut>trad. M. Blahu&scirc;</aut>,
          <vrk><url ref="http://filmoj.net/kusxejetoj">
          Ku&scirc;ejetoj</url></vrk>,
          <lok>[2009]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_d;&c_a;&c_n;</trd>,
    <trd>&c_f;&c_ib;&c_m;&c_ib;&c_ja;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>arabsk&eacute; kadidlo</trd>,
    <trd>olibanum</trd>,
    <trd>olib&aacute;n</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Olibanum</trd>,
     <trd>Weihrauchharz</trd>
  </trdgrp>
  <trd lng="en">frankincense</trd>
  <trdgrp lng="fr">
    <trd>encens naturel</trd>,
    <trd>oliban</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>jegenyefeny&odblac;-gyanta</trd>,
     <trd>feny&odblac;t&ouml;mj&eacute;n</trd>
  </trdgrp>
  <trd lng="ja">&#x4E73;&#x9999;  <pr>&#x306B;&#x3085;&#x3046;&#x3053;&#x3046;</pr></trd>
  <trdgrp lng="lat">
    <trd>incensum album</trd>,
    <trd>Thus album</trd>
  </trdgrp>
  <trd lng="nl">wierook</trd>
  <trd lng="pl">olibanum</trd>
  <trd lng="pt">ol&iacute;bano</trd>
  <trd lng="ro">t&abreve;m&acirc;ie</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_d;&c_a;&c_n;</trd>,
    <trd>&c_f;&c_i;&c_m;&c_i;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>arabsk&eacute; kadidlo</trd>,
    <trd>olib&aacute;n</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x430;&#x434;&#x430;&#x43D;</trd>,
    <trd>&#x444;&#x456;&#x43C;&#x456;&#x430;&#x43C;</trd>,
    <trd>&#x430;&#x440;&#x43E;&#x43C;&#x430;&#x442;&#x438;&#x447;&#x43D;&#x430; &#x441;&#x43C;&#x43E;&#x43B;&#x430;</trd>
  </trdgrp>
  <trd lng="zh">&#x4E73;&#x9999; <pr>r&#x1D4;xi&#x101;ng</pr></trd>
</drv>
</art>
