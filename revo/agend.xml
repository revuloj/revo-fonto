<vortaro>
<art mrk="$Id$">
  <kap><rad>agend</rad>/o<fnt>Z</fnt></kap>

<drv mrk="agend.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Kajero en kiu oni notas la&ubreve; &ccirc;iu tago la farotajn  aferojn.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_t;&c_a;&c_t;&c_n;&c_ib;&c_k;</trd>, 
    <trd>&c_z;&c_a;&c_p;&c_ib;&c_s;&c_n;&c_a;&c_ja; 
      &c_k;&c_n;&c_ib;&c_zh;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">agenda</trd>
  <trdgrp lng="cs">
    <trd>di&aacute;&rcaron;</trd>,
    <trd>kniha agendy</trd>,
    <trd>prac. n&aacute;pl&ncaron;</trd>,
    <trd>z&aacute;pisn&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Terminkalender</trd>,
     <trd>Kalender</trd>,
     <trd>Vormerkbuch</trd>,
     <trd>Vormerkkalender</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>memorandum book</trd>,
    <trd>engagement diary</trd>,
    <trd>notebook</trd>
  </trdgrp>
  <trd lng="es">agenda</trd>
  <trd lng="fr">agenda</trd>
  <trdgrp lng="hu">
     <trd>hat&aacute;rid&odblac;napl&oacute;</trd>,
     <trd>notesz</trd>
  </trdgrp>
  <trd lng="it">agenda</trd>
  <trd lng="nl">agenda</trd>
  <trdgrp lng="pl">
    <trd>terminarz</trd>,
    <trd>agenda</trd>
  </trdgrp>
  <trd lng="pt">agenda</trd>
  <trd lng="ru">&c_z;&c_a;&c_p;&c_i;&c_s;&c_n;&c_a;&c_ja; 
    &c_k;&c_n;&c_i;&c_zh;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>notes</trd>,
    <trd>z&aacute;znamn&iacute;k denn&yacute;ch &uacute;loh</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>masa takvimi</trd>,
    <trd>ajanda</trd>
  </trdgrp>
</drv>
</art>
