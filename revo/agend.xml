<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: agend.xml,v 1.14 2021/02/28 14:48:40 revo Exp $">
  <kap><rad>agend</rad>/o<fnt>Z</fnt></kap>

<drv mrk="agend.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="regist.0o.dokumento">Registro</ref>
      de la farenda&jcirc;oj:
      <ekz>
        tiu libro estis siatempe furore akceptata, kiel proponata <tld/>o por socia reformo
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="https://www.monato.be/2005/008649.php">Utopia verko dekna&ubreve;jarcenta</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la nombro de individuoj kun dekstremaj vidpunktoj kaj <tld/>oj &scirc;ajnas kreski
        <fnt>
          <bib>Monato</bib>
          <aut>Ionel Onet</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007036.html">Ekstremismo:
         Pri kaldronoj kaj potoj: &ccirc;u amba&ubreve; kotaj?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        vi povas per <klr>[po&scirc;telefono]</klr> telefoni, krozi en Interreto, 
        kontroli vian retpo&scirc;ton, sendi tekstomesa&gcirc;ojn <klr>[&#x2026;]</klr> kontroli 
        vian <tld/>on, a&ubreve;skulti vian &scirc;atatan muzikon, rigardi filmojn, kaj kaj kaj
        <fnt>
          <bib>Monato</bib>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="https://www.monato.be/2008/009612.php">Inteligenta po&scirc;amiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        lokaj administradestroj kaj politikistoj, kiuj kune cerbumis por
        provi respekti la <tld lit="A"/>o-21-kontrakton
        <fnt>
          <bib>Monato</bib>
          <aut>&Jcirc;anin Ratu&scirc;njak</aut>
          <vrk><url ref="https://www.monato.be/2010/009980.php">Senpaga 
          transporto danke al lo&gcirc;antarkonscii&gcirc;o</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_t;&c_a;&c_t;&c_n;&c_ib;&c_k;</trd>, 
    <trd>&c_z;&c_a;&c_p;&c_ib;&c_s;&c_n;&c_a;&c_ja; 
      &c_k;&c_n;&c_ib;&c_zh;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">agenda</trd>
  <trdgrp lng="cs">
    <trd>di&aacute;&rcaron;</trd>,
    <trd>kniha agendy</trd>,
    <trd>prac. n&aacute;pl&ncaron;</trd>,
    <trd>z&aacute;pisn&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Agenda</trd>,
    <trd>Todo-Liste</trd>,
    <trd>Programm</trd>,
    <trd>Tagesordnung</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>memorandum book</trd>,
    <trd>engagement diary</trd>,
    <trd>notebook</trd>
  </trdgrp>
  <trd lng="es">agenda</trd>
  <trd lng="fr">agenda</trd>
  <trdgrp lng="he">
    <trd>&#x5D9;&#x5D5;&#x5DE;&#x5DF;</trd>,
    <trd>&#x5DC;&#x5D5;&#x5D7; &#x5D6;&#x5DE;&#x5E0;&#x5D9;&#x5DD;</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>hat&aacute;rid&odblac;napl&oacute;</trd>,
     <trd>notesz</trd>
  </trdgrp>
  <trd lng="it">agenda</trd>
  <trd lng="nl">agenda</trd>
  <trdgrp lng="pl">
    <trd>terminarz</trd>,
    <trd>agenda</trd>
  </trdgrp>
  <trd lng="pt">agenda</trd>
  <trd lng="ru">&c_z;&c_a;&c_p;&c_i;&c_s;&c_n;&c_a;&c_ja; 
    &c_k;&c_n;&c_i;&c_zh;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>notes</trd>,
    <trd>z&aacute;znamn&iacute;k denn&yacute;ch &uacute;loh</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>masa takvimi</trd>,
    <trd>ajanda</trd>
  </trdgrp>
</drv>
</art>
