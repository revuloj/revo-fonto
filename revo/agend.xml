<art mrk="$Id$">
  <kap><rad>agend</rad>/o<fnt>Z</fnt></kap>

<drv mrk="agend.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Kajero en kiu oni notas la&ubreve; &ccirc;iu tago la farotajn  aferojn.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_t;&c_a;&c_t;&c_n;&c_ib;&c_k;</trd>, 
    <trd>&c_z;&c_a;&c_p;&c_ib;&c_s;&c_n;&c_a;&c_ja; 
      &c_k;&c_n;&c_ib;&c_zh;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>memorandum book</trd>,
    <trd>notebook</trd>
  </trdgrp>
  <trd lng="fr">agenda</trd>
  <trd lng="it">agenda</trd>
  <trd lng="nl">agenda</trd>
  <trd lng="pt">agenda</trd>
  <trd lng="ru">&c_z;&c_a;&c_p;&c_i;&c_s;&c_n;&c_a;&c_ja; 
    &c_k;&c_n;&c_i;&c_zh;&c_k;&c_a;</trd>
</drv>
</art>
