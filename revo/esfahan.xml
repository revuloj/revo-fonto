<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: esfahan.xml,v 1.3 2022/04/27 14:38:54 afido Exp $">
    <kap>
      <rad>Esfahan</rad>/o
    </kap>

<drv mrk="esfahan.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Granda <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref> en <ref tip="malprt"
      cel="iran.I0o">Irano</ref>,
      &ccirc;. 400&#8239;km sude de <ref cel="teheran.0o">Teherano</ref>:
      <ekz>
        <tld/>o <klr>[&#8230;]</klr> iam estis la &ccirc;efurbo de Irano kaj unu el la plej grandaj
        urboj de la mondo <klr>[&#8230;]</klr>, &gcirc;i floris inter 1050-1722
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Esfahano">Esfahano</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estas agrabla urbo, kaj tie mi anka&ubreve; renkontis multajn aliajn
        biciklistojn kaj voja&gcirc;antojn
        <fnt>
          <vrk><url ref="https://www.b-no.ch/texte/ze12.pdf">La voja&gcirc;o de la ru&gcirc;a
          biciklo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_Ib;&c_s;&c_f;&c_a;&c_g;&c_a;&c_n;</trd>,
      <trd>&c_Ib;&c_s;&c_f;&c_a;&c_h;&c_a;&c_n;</trd>
    </trdgrp>
    <trd lng="de">Isfahan</trd>
    <trd lng="en">Isfahan</trd>
    <trd lng="fr">Ispahan</trd>
    <trd lng="fa">&a_A;&a_S;&a_f;&a_h;&a_A;&a_n;</trd>
  </snc>
</drv>

  </art>

