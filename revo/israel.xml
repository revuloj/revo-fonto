<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: israel.xml,v 1.25 2021/05/14 07:54:33 revo Exp $">
<kap>
<ofc>9</ofc><rad>Israel</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="israel.I0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="israel.I0o.GEOG">
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" cel="sxtat.0o" lst="voko:&scirc;tatoj">&Scirc;tato</ref>
      situanta en
      okcidenta <ref tip="malprt" cel="azi.A0o">Azio</ref>
      kaj limanta okcidente al
      <ref tip="vid" cel="mediterane.M0o">Mediteraneo</ref>, norde al
      <ref tip="vid" cel="liban.L0o">Libano</ref>, nord-oriente al
      <ref tip="vid" cel="siri.S0o">Sirio</ref>, oriente
      al <ref tip="vid" cel="jordan.J0o">Jordanio</ref>, sude al la
      <ref tip="vid" cel="mar.RugxaM0o">Ru&gcirc;a Maro</ref>,
      sud-okcidente al <ref tip="vid" cel="egipt.E0ujo">Egiptujo</ref>;
      kun <ref tip="vid" cel="urb.cxef0o">&ccirc;efurbo</ref> kaj
      plej granda urbo <ref tip="prt" cel="jerusa.J0o">Jerusalemo</ref>:
      <ekz>
        pli ol 3000 judoj ricevis la eblecon repatruji&gcirc;i al <tld/>o danke la internacian
        kristanan fonduson pri helpo al judoj &leftquot;<nom>Aven-Ezer</nom>&rightquot;
        <fnt>
          <bib>Monato</bib>
          <vrk><url
          ref="http://steloj.de/esperanto/monato1/monato-9310-artikoletoj.txt">Malkopiilo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        dek du norveginoj iniciatis Internacian Virinan Mar&scirc;on por Homaj Rajtoj en <tld/>o kaj
        Palestino
        <fnt>
          <bib>Monato</bib>
          <aut>Douglas Draper</aut>
          <vrk><url ref="https://www.monato.be/2003/008001.html">Pa&scirc;oj al paco</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la siriaj Golan-altebena&jcirc;oj estis konkeritaj de <tld/>o en 1967 anka&ubreve; pro la fakto, ke tie
        trovi&gcirc;as unu el la fontoj de Jordan-rivero
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006866.html">La blua "oro" &ccirc;iam
          malsufi&ccirc;os</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="izrael.0o.praregno">Izraelo<sncref/></ref>
  <trd lng="be">&c_Ib;&c_z;&c_r;&c_a;&c_ib;&c_l;&c_mol;</trd>
  <trd lng="bg">&c_I;&c_z;&c_r;&c_a;&c_je;&c_l;</trd>
  <trdgrp lng="ca">
    <trd>Israel</trd>,
    <trd>Isr&amacron;'&imacron;l</trd>
  </trdgrp>
  <trd lng="de">Israel</trd>
  <trd lng="es">Israel</trd>
  <trdgrp lng="fr">
    <trd>Isra&euml;l</trd>,
    <trd>Etat d'Isra&euml;l</trd>
  </trdgrp>
  <trd lng="en">Israel</trd>
  <trd lng="he">&yod;&shin;&resh;&alef;&lamed;</trd>
  <trd lng="id">Israel</trd>
  <trd lng="nl">Isra&euml;l</trd>
  <trd lng="no">Israel</trd>
  <trd lng="pl">Izrael</trd>
  <trd lng="pt">Israel</trd>
  <trd lng="ru">&c_I;&c_z;&c_r;&c_a;&c_i;&c_l;&c_mol;</trd>
</drv>

<drv mrk="israel.i0a">
  <kap><tld lit="i"/>a</kap>
  <snc mrk="israel.i0a.GEOG">
    <dif>
      Rilata al <tld lit="i"/>anoj, ilia nacieco a&ubreve; &scirc;tato:
      <ekz>
        la mar&scirc;antinoj gastos &ccirc;e israelaj kaj palestinaj virinoj
        <fnt>
          <bib>Monato</bib>
          <aut>Douglas Draper</aut>
          <vrk><url ref="https://www.monato.be/2003/008001.html">Pa&scirc;oj al paco</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld lit="i"/>aj turistoj rezignos pri siaj ferioj
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006884.html">A&ubreve;strio: &Ccirc;u veneno en
          Vieno?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        supera <tld lit="i"/>a a&scirc;kenaza rabeno
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007207.html">La mezepokaj judaj
          mortintoj fine trankvilaj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve; la <tld lit="i"/>a &ccirc;efministro
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007209.html">A&ubreve;strio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_z;&c_r;&c_a;&c_ib;&c_l;&c_mol;&c_s;&c_k;&c_ib;</trd>
  <trdgrp lng="ca">
    <trd>israelita</trd>,
    <trd>hebreu <klr>(adj.)</klr></trd>
  </trdgrp>
  <trd lng="de">israelisch</trd>
  <trd lng="fr">isra&euml;lien<klr> (adj.)</klr></trd>
  <trd lng="en">Israeli</trd>
  <trd lng="he">&yod;&shin;&resh;&alef;&lamed;&yod;</trd>
  <trd lng="nl">Isra&euml;lisch</trd>
  <trd lng="no">israelsk</trd>
  <trd lng="pl">izraelski</trd>
  <trdgrp lng="pt">
   <trd>israel&iacute;tico<klr tip="ind"> (adj.)</klr></trd>, 
   <trd>israelense<klr tip="amb"> (adj.)</klr></trd> 
  </trdgrp>
  <trd
  lng="ru">&c_i;&c_z;&c_r;&c_a;&c_i;&c_l;&c_mol;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="israel.i0ano">
  <kap><tld lit="i"/>ano</kap>
  <snc mrk="israel.i0ano.POL">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="israel.I0o"><tld/>o</ref>:
      <ekz>
        sabraoj, denaskaj <tld lit="i"/>anoj
        <fnt>
          <bib>Monato</bib>
          <aut>H&eacute;l&egrave;ne Falk</aut>
          <vrk><url ref="https://www.monato.be/2003/008038.html">Israelanoj instruis
          terorismon</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="i"/>anoj ne hezitis aneksi orientan Jerusalemon kaj la siriajn teritoriojn,
        kiujn ili okupas
        <fnt>
          <bib>Monato</bib>
          <aut>Eskil SVANE</aut>
          <vrk><url ref="https://www.monato.be/2003/008197.html">Misinformaj komentoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_z;&c_r;&c_a;&c_ib;&c_l;&c_mol;&c_c;&c_ja;&c_n;&c_ib;&c_n;</trd>
  <trdgrp lng="ca">
    <trd>israelita</trd>,
    <trd>hebreu <klr>(subst.)</klr></trd>
  </trdgrp>
  <trd lng="de">Israeli</trd>
  <trd lng="fr">Isra&euml;lien<klr> (subst.)</klr></trd>
  <trd lng="en">Israeli</trd>
  <trd lng="he">&yod;&shin;&resh;&alef;&lamed;&yod;</trd>
  <trd lng="nl">Isra&euml;li</trd>
  <trd lng="no">israelier</trd>
  <trd lng="pl">Izraelczyk</trd>
  <trd lng="pt">israelense<klr tip="amb"> (pessoa)</klr></trd>
  <trd
  lng="ru">&c_i;&c_z;&c_r;&c_a;&c_i;&c_l;&c_mol;&c_t;&c_ja;&c_n;&c_i;&c_n;</trd>
</drv>

</art>

