<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>Israel</rad>/o
</kap>

<drv mrk="israel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="sxtat.0o">&Scirc;tato</ref> situanta en
      okcidenta <ref tip="malprt" cel="azi.A0o">Azio</ref>
      kaj limanta okcidente al
      <ref tip="vid" cel="mediterane.0o">Mediteraneo</ref>, norde al
      <ref tip="vid" cel="liban.0o">Libano</ref>, nord-oriente al
      <ref tip="vid" cel="sirio.0o">Sirio</ref>, oriente
      al <ref tip="vid" cel="jordan.0o">Jordanio</ref>, sude al la
      <ref tip="vid" cel="rugx.R0aMaro">Ru&gcirc;a Maro</ref>,
      sud-okcidente al <ref tip="vid" cel="egipt.E0ujo">Egiptujo</ref>.
      <ref tip="prt" cel="jerusa.0o">Jerusalemo</ref> estas la
      <ref tip="vid" cel="urb.cxef0o">&ccirc;efurbo</ref> kaj
      plej granda urbo.
    </dif>
  </snc>
  <trd lng="en">Israel</trd>
  <trd lng="he">&yod;&shin;&resh;&alef;&lamed;</trd>
  <trd lng="pt">Israel</trd>
  <trd lng="ru">&c_I;&c_z;&c_r;&c_a;&c_i;&c_l;&c_mol;</trd>
</drv>

<drv mrk="israel.i0a">
  <kap><tld lit="i"/>a</kap>
  <snc>
    <dif>
      Rilata al <tld lit="i"/>anoj, ilia nacieco a&ubreve; &scirc;tato.
    </dif>
  </snc>
  <trd lng="en">Israeli</trd>
  <trd lng="he">&yod;&shin;&resh;&alef;&lamed;&yod;</trd>
  <trdgrp lng="pt">
   <trd>israel&iacute;tico<klr tip="ind"> (adj.)</klr></trd>, 
   <trd>israelense<klr tip="amb"> (adj.)</klr></trd> 
  </trdgrp>
  <trd lng="ru">&c_i;&c_z;&c_r;&c_a;&c_i;&c_l;&c_mol;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="israel.i0ano">
  <kap><tld lit="i"/>ano</kap>
  <snc>
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="israel.0o"><tld/>o</ref>.
    </dif>
  </snc>
  <trd lng="en">Israeli</trd>
  <trd lng="he">&yod;&shin;&resh;&alef;&lamed;&yod;</trd>
  <trd lng="pt">israelense<klr tip="amb"> (pessoa)</klr></trd>
  <trd lng="ru">&c_i;&c_z;&c_r;&c_a;&c_i;&c_l;&c_mol;&c_t;&c_ja;&c_n;&c_i;&c_n;</trd>
</drv>

</art>

