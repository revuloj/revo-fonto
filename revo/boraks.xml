<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: boraks.xml,v 1.22 2023/10/14 08:39:48 revo Exp $">
  <kap>
    <ofc>*</ofc>
    <rad>boraks</rad>/o
  </kap>
<drv mrk="boraks.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Natria <ref tip="super" cel="sal.0o.KEM">salo</ref>
      de <ref tip="prt" cel="bor1.0o">boro</ref>, 
      <frm>Na<sub>2</sub>B<sub>4</sub>O<sub>7</sub></frm>:
      <ekz>
        por igi <klr>[polimeron]</klr> pli efika, oni normale &gcirc;in retigas 
        (interligante la &ccirc;enojn) per bor-acido a&ubreve; <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Werner Fuss</aut>
          <vrk><url ref="&Monato;/2014/011348.php">Novtipa gasminado: &ccirc;u vere
          dan&gcirc;era?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Samuel Hahnemann</nom> <klr>[&#x2026;]</klr> vendis memproduktitan novan bazan salon,
        kiu evidenti&gcirc;is esti la delonge konata <tld/>o, li konfesis sian eraron kaj redonis la
        ricevitan monon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=35751">Samuel Hahnemann</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_b;&c_o;&c_r;&c_a;&c_k;&c_s;</trd>
    <trd lng="ca">b&ograve;rax</trd>
    <trdgrp lng="de">
      <trd>Borax</trd>,
      <trd>Tinkal</trd>,
      <trd>Natriumborat</trd>
    </trdgrp>
    <trd lng="en">borax</trd>
    <trd lng="es">b&oacute;rax</trd>
    <trd lng="fr">borax</trd>
    <trd lng="nl">borax</trd>
    <trd lng="pl">boraks</trd>
    <trd lng="pt">b&oacute;rax</trd>
    <trd lng="ru">&c_b;&c_u;&c_r;&c_a;</trd>
  </snc>
    <trdgrp lng="be">
      <trd>&c_b;&c_o;&c_r;&c_a;&c_k;&c_s;</trd>,
      <trd>&c_b;&c_a;&c_r;&c_a;&c_k;&c_s;</trd>
    </trdgrp>
  <trd lng="cs">borax</trd>
  <trd lng="he">&#x5D1;&#x5BC;&#x5D5;&#x5B9;&#x5E8;&#x5B8;&#x5E7;&#x5B0;&#x5E1;</trd>
  <trd lng="ja">&#x30DB;&#x30A6;&#x7802;  <pr>&#x307B;&#x3046;&#x3057;&#x3083;</pr></trd>
  <trd lng="sk">b&oacute;rax</trd>
  <trd lng="uk">&#x431;&#x443;&#x440;&#x430;</trd>
</drv>
</art>
