<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: piraruku.xml,v 1.7 2024/02/17 06:43:08 revo Exp $">
<kap>
  <rad>piraruku</rad>/o
</kap>
<drv mrk="piraruku.0o">
  <kap><tld/>o</kap>
  <snc mrk="piraruku.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Amazonia tre granda riverfi&scirc;o kun &scirc;ate man&gcirc;ata karno,
      el familio osteoglosedoj
      (<trd lng="la">Arapaima gigas</trd>):
      <ekz>
        hodia&ubreve; vespere pasis granda fi&scirc;aro da <tld/>oj. Dudek unu 
        pirogoj kaj du boatoj eliris por fi&scirc;kaptado
        <fnt>
          <aut>R. da Silva Ribeiro</aut>,
          <vrk>Voja&gcirc;taglibro al Amazonio</vrk>,
          <lok><url ref="http://www.lakaravelo.com/LK_000/LK_018.pdf">
          La Karavelo, 2009-09 (18)</url>, p. 15a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o <klr>...</klr> estas alnomata <ctl>la gado el 
        Amazonio</ctl>
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Pirakukuo">Pirarukuo</url>,
          2010-01-17</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ib;&c_r;&c_a;&c_k;&c_u;&c_k;&c_u;</trd>,
    <trd>&c_a;&c_r;&c_a;&c_p;&c_a;&c_j;&c_m;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_i;&c_r;&c_a;&c_r;&c_u;&c_k;&c_o;</trd>
  <trd lng="de">Arapaima</trd>
  <trd lng="en">arapaima</trd>
  <trdgrp lng="es">
    <trd>arapaima</trd>,
    <trd>piraruc&uacute;</trd>, 
    <trd>paiche</trd>
  </trdgrp> 
  <trdgrp lng="fr">
    <trd>pa&iuml;che</trd>,
    <trd>pirarucu</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>arapaime</trd>,
    <trd>piraruku</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5DE8;&#x9AA8;&#x820C;&#x9B5A; <pr>j&#xF9;g&#x1D4;sh&#xE9;y&#xFA;</pr></trd>,
    <trd>&#x5DE8;&#x9AA8;&#x820C;&#x9C7C; <pr>j&#xF9;g&#x1D4;sh&#xE9;y&#xFA;</pr></trd>
  </trdgrp>
</drv>
</art>

