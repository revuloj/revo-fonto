<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc> <rad>ar&hcirc;iv</rad>/o,
  <var><kap>
    <ofc>3</ofc> <rad>arkiv</rad>/o
  </kap></var>
</kap>
<drv mrk="arhxiv.0o">
  <kap><tld/>o,
    <var><kap>arkivo</kap></var>
  </kap>
  <snc mrk="arhxiv.0o.dokumentaro">
    <dif>
      Kolekto da malnovaj aktoj kaj dokumentoj:
      <ekz>
        konsulti la arkivon de la Ligo de Nacioj
        <fnt>
          <bib>Monato</bib>
          <vrk><url
          ref="https://www.esperanto.be/fel/mon/006947.html">Reago:
          Eks-Jugoslavio: kiu kiun mistifikas?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la arkivo de liaj notlibroj, leteroj, raportoj
        <fnt>
          <aut>Wim M. A. De Smet</aut>: <vrk>Anta&ubreve; cent jaroj</vrk>,
          <bib>Monato</bib>
          <lok>jaro 1998a, numero 5a, p. 25a</lok>
        </fnt>;
      </ekz>
      <ekz>
        disponigi al la ser&ccirc;istoj <tld/>ajn dokumentojn
        <fnt>
          <aut>Robert Roze</aut>: <vrk>Verkistino - orientalistino -
          esploristino</vrk>,
          <bib>Monato</bib>
          <lok>jaro 1999a, numero 1a, p. 17a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Archiv</trd>,
      <trd>Urkundensammlung</trd>,
      <trd>Dokumentensammlung</trd>
    </trdgrp>
  </snc>
  <snc mrk="arhxiv.0o.arhxivejo">
    <dif>
      <ref tip="dif" cel="arhxiv.0ejo"><tld lit="A"/>ejo</ref>:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        formeti en <tld/>on
        <klr>(flankenlasi, forgesi)</klr>
        <fnt>
          <bib>Prv</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kvankam nun mi jam apartenas al la <tld/>o
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 21a, p.
        126a</lok></fnt>
        <klr>(jam estas eksmoda)</klr>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Archiv</trd>,
      <trd>Archivgeb&auml;ude</trd>,
      <trd>Urkundenstelle</trd>,
      <trd>Dokumentationsstelle</trd>
    </trdgrp>
    <adm>la ekzemplo estas miscitita -> &gcirc;i apartenas al ar&hcirc;ivo</adm>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_h;&c_ib;&c_w;</trd>
  <trd lng="ca">arxiu</trd>
  <trdgrp lng="de">
    <trd>Archiv</trd>
  </trdgrp>
  <trd lng="en">archive</trd>
  <trd lng="es">archivo</trd>
  <trdgrp lng="hu">
    <trd>lev&eacute;lt&aacute;r</trd>,
    <trd>arch&iacute;vum</trd>,
    <trd>okm&aacute;nyt&aacute;r</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>archives</trd>
  </trdgrp>
  <trd lng="id">arsip</trd>
  <trd lng="nl">archief</trd>
  <trd lng="ru">&c_a;&c_r;&c_h;&c_i;&c_v;</trd>
  <trd lng="sk">arch&iacute;v</trd>
</drv>

<drv mrk="arhxiv.0ejo">
  <kap><tld/>ejo,
    <var><kap>arkivejo</kap></var>
  </kap>
  <snc>
    <dif>
      Loko, kie oni gardas malnovajn dokumentojn.
    </dif>
    <ref cel="arhxiv.0o.arhxivejo"><tld/>o<sncref/></ref>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_h;&c_ib;&c_w;</trd>
  <trd lng="ca">arxiu <klr>(lloc)</klr></trd>
  <trdgrp lng="de">
    <trd>Archiv</trd>,
    <trd>Archivgeb&auml;ude</trd>,
    <trd>Urkundenstelle</trd>,
    <trd>Dokumentationsstelle</trd>
  </trdgrp>
  <trd lng="en">archive</trd>
  <trd lng="es">archivo</trd>
  <trdgrp lng="fr">
    <trd>archives<klr> (lieu)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>lev&eacute;lt&aacute;r</trd>,
    <trd>arch&iacute;vum</trd>,
    <trd>okm&aacute;nyt&aacute;r</trd>
  </trdgrp>
  <trd lng="nl">archief</trd>
  <trd lng="ru">&c_a;&c_r;&c_h;&c_i;&c_v;</trd>
  <trd lng="sk">arch&iacute;v</trd>
</drv>

<drv mrk="arhxiv.0isto">
  <kap><tld/>isto,
    <var><kap>arkivisto</kap></var>
  </kap>
  <snc>
    <dif>
      Oficisto gardanta, aran&gcirc;anta <tld/>ojn:
      <ekz>
        hungara historiisto, arkivisto,...
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=447490">Andr&aacute;s
          Kom&aacute;romy</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;efarkivisto de la urba <tld/>o
        <fnt>
          <aut>Roland Rotsaert</aut>: <vrk>Mezepoka Hanso-asocio
          revivi&gcirc;as</vrk>,
          <bib>Monato</bib>
          <lok>jaro 2002a, numero 4a, p. 17a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_r;&c_h;&c_ib;&c_v;&c_a;&c_r;&c_y;&c_ju;&c_s;</trd>,
    <trd>&c_a;&c_r;&c_h;&c_ib;&c_v;&c_ib;&c_s;&c_t;</trd>
  </trdgrp>
  <trd lng="de">Archivar</trd>
  <trd lng="en">archivist</trd>
  <trd lng="es">archivista</trd>
  <trd lng="fr">archiviste</trd>
  <trd lng="hu">lev&eacute;lt&aacute;ros</trd>
  <trd lng="nl">archivaris</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_h;&c_i;&c_v;&c_i;&c_s;&c_t;</trd>,
    <trd>&c_a;&c_r;&c_h;&c_i;&c_v;&c_a;&c_r;&c_i;&c_u;&c_s;</trd>
  </trdgrp>
  <trd lng="sk">archiv&aacute;r</trd>
</drv>

<drv mrk="arhxiv.0i">
  <kap><tld/>i,
    <var><kap>arkivi</kap></var>
  </kap>
  <snc>
    <dif>
      Meti en <tld/>on:
      <ekz>
        ili sendas siajn kontribuojn retpo&scirc;te al servilo, kiu <tld/>as
        &ccirc;ion en datumbazo
        <fnt>
          <aut>Alex Mikishev</aut>: <vrk>Astronomia Terminaro</vrk>.
          <bib>LOdE</bib>, <lok>2000, n-ro 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel ni arkivu la ekzistantajn materialojn?
        <fnt>
          <aut>Marc van Oostendorp</aut>: <vrk>Esperantologio
          komputile</vrk>.
          <bib>LOdE</bib>, <lok>1999, n-ro 4</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">archivieren</trd>
  <trd lng="en">to archive</trd>
  <trd lng="es">archivar</trd>
  <trd lng="fr">archiver</trd>
  <trd lng="nl">archiveren</trd>
</drv>
</art>
