<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>ar&hcirc;iv</rad>/o
</kap>
<drv mrk="arhxiv.0o">
  <kap><tld/>o</kap>
  <snc>
    <ref tip="dif" cel="arkiv.0o">arkivo</ref>
    <ekz>
      <uzo tip="stl">FIG</uzo>
      kvankam nun mi jam apartenas al la ar&hcirc;ivo
      <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 21a, p. 126a</lok></fnt>
      <klr>(jam estas eksmoda)</klr>.
    </ekz>
    <ekz>
      disponigi al la ser&ccirc;istoj <tld/>ajn dokumentojn<fnt><aut>Robert
      Roze</aut>: <vrk>Verkistino - orientalistino - esploristino</vrk>,
      <bib>Monato</bib><lok>jaro 1999a, numero 1a, p. 17a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="de">
    <trd>Archiv</trd>
  </trdgrp>
  <trd lng="en">archive</trd>
  <trd lng="es">archivo</trd>
  <trdgrp lng="de">
    <trd>lev&eacute;lt&aacute;r</trd>,
    <trd>arch&iacute;vum</trd>,
    <trd>okm&aacute;nyt&aacute;r</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>archives</trd>
  </trdgrp>
  <trd lng="nl">archief</trd>
  <trd lng="ru">&c_a;&c_r;&c_h;&c_i;&c_v;</trd>
  <trd lng="sk">arch&iacute;v</trd>
</drv>

<drv mrk="arhxiv.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <ref tip="dif" cel="arkiv.0ejo">arkivejo</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Archiv</trd>,
    <trd>Archivgeb&auml;ude</trd>,
    <trd>Urkundenstelle</trd>,
    <trd>Dokumentationsstelle</trd>
  </trdgrp>
  <trd lng="en">archive</trd>
  <trd lng="es">archivo</trd>
  <trdgrp lng="fr">
    <trd>archives<klr> (lieu)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>lev&eacute;lt&aacute;r</trd>,
    <trd>arch&iacute;vum</trd>,
    <trd>okm&aacute;nyt&aacute;r</trd>
  </trdgrp>
  <trd lng="nl">archief</trd>
  <trd lng="ru">&c_a;&c_r;&c_h;&c_i;&c_v;</trd>
  <trd lng="sk">arch&iacute;v</trd>
</drv>

<drv mrk="arhxiv.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <ref tip="dif" cel="arkiv.0isto">arkivisto</ref>
  </snc>
  <trd lng="de">Archivar</trd>
  <trd lng="en">archivist</trd>
  <trd lng="es">archivista</trd>
  <trdgrp lng="fr">
    <trd>archiviste</trd>
  </trdgrp>
  <trd lng="hu">lev&eacute;lt&aacute;ros</trd>
  <trd lng="nl">archivaris</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_h;&c_i;&c_v;&c_i;&c_s;&c_t;</trd>,
    <trd>&c_a;&c_r;&c_h;&c_i;&c_v;&c_a;&c_r;&c_i;&c_u;&c_s;</trd>
  </trdgrp>
  <trd lng="sk">archiv&aacute;r</trd>
</drv>
</art>
