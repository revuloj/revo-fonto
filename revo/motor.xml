    <dif>
      Speco de <ref tip="super" cel="motor.0o.TEK">motoro</ref>
      en kiu eksplodoj de miksa&jcirc;o de brula&jcirc;o kaj aero
      pu&scirc;adas pi&scirc;tojn.
    </dif>
    <refgrp tip="sub">
      <ref cel="dizel.0o.motoro">dizelo<sncref/></ref>,
      <ref cel="karbur.0a">karbura motoro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_u;&c_h;&c_a;&c_v;&c_ib;&c_k;
&c_u;&c_n;&c_u;&c_t;&c_r;&c_a;&c_n;&c_a;&c_g;&c_a;
&c_z;&c_g;&c_a;&c_r;&c_a;&c_n;&c_mol;&c_n;&c_ja;</trd>
