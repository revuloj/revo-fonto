<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>motor</rad>/o
</kap>
<drv mrk="motor.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Ma&scirc;ino, per kiu iu forto povas produkti movon:
      <ekz>
        elektra <tld/>o.
      </ekz>
    </dif>
    <trd lng="ru">&c_m;&c_o;&c_t;&c_o;&c_r;</trd>
    <trd lng="sv">motor</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tiu a&ubreve; tio, kiu instigas al ia agado a&ubreve; progreso:
      <ekz>
        la lingvo estas la &ccirc;efa <tld/>o de la civilizacio<fnt>Z</fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
     <trd>motor</trd>,
     <trd>engine</trd>
  </trdgrp>
  <trd lng="hu">motor</trd>
  <trd lng="nl">motor</trd>
  <trd lng="ru">&c_d;&c_v;&c_i;&c_g;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>  
</drv>

<drv mrk="motor.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Homo, kiu funkciigas a&ubreve;tomobilan <tld/>on
    </dif>
    <refgrp tip="sin">
      <ref cel="sxofor.0o">&scirc;oforo</ref>,
      <ref cel="auxt.0isto">a&ubreve;tisto</ref>.
    </refgrp>
  </snc>
  <trd lng="en">motorist</trd>
  <trd lng="hu">aut&oacute;s</trd>
  <trd lng="nl">automobilist</trd>
  <trd lng="ru">&c_m;&c_o;&c_t;&c_o;&c_r;&c_i;&c_s;&c_t;</trd>
</drv>

<drv mrk="motor.eksplod0o">
  <kap>eksplod<tld/>o</kap>
  <snc>
    <dif>
       Speco de motoro en kiu eksplodoj de miksa&jcirc;o de brula&jcirc;o kaj aero pu&scirc;adas <ref tip="dif" cel="pisxt.0o">pi&scirc;tojn</ref>. 
    </dif>
  </snc>
  <trd lng="en">internal combustion engine</trd>
</drv>

<drv mrk="motor.vapor0o">
  <kap>vapor<tld/>o</kap>
  <snc>
    <dif>
       Speco de motoro kiu uzas akvan vaporon por pu&scirc;adi <ref tip="dif" cel="pisxt.0o">pi&scirc;tojn</ref>. 
    </dif>
    <refgrp tip="sin">
      <ref cel="lokomo.0o">lokomobilo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="en">
     <trd>steam engine</trd>,
     <trd>external combustion engine</trd>
  </trdgrp>
</drv>
</art>
