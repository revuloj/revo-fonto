<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>1</ofc>
      <rad>motor</rad>/o
    </kap>

    <drv mrk="motor.0o">
      <kap><tld/>o</kap>
      <snc mrk="motor.0o.TEK">
        <uzo tip="fak">TEK</uzo>
        <dif>
          Ma&scirc;ino servanta por transformi ian energion (precipe
          elektran a&ubreve; &hcirc;emian) en me&hcirc;anikan laboron:
          <ekz>
            la &scirc;oforo, kolermiena, startigis la <tld/>on de sia kamiono
            <fnt><bib>MkM</bib>, <lok>&ccirc;ap.5a</lok></fnt>;
          </ekz><ekz>
            kvar elektraj <tld/>oj trovigxas sub la &scirc;ipo <klr>...</klr>
            gasturbinoj, similaj al tiuj de grandkapacitaj avioj, kaj
            pli puraj kaj silentaj ol dizelaj <tld/>oj, estas uzataj
            por produkti la elektran energion por la <tld/>oj
            <fnt>
              <aut>Christian BERTIN</aut>:
              <vrk>Re&gcirc;ino de la maroj</vrk>. <bib>Monato</bib>
            </fnt>.
          </ekz>
        </dif>
        <trd lng="hu">motor</trd>    
        <trdgrp lng="ru">
          <trd>&c_d;&c_v;&c_i;&c_g;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
          <trd>&c_m;&c_o;&c_t;&c_o;&c_r;</trd>
        </trdgrp>
        <trd lng="sv">motor</trd>
      </snc>

      <snc mrk="motor.0o.FIG">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Tiu a&ubreve; tio, kiu instigas al ia agado a&ubreve; progreso:
          <ekz>
            la lingvo ja estas la &ccirc;efa <tld/>o de la civilizacio
	    <fnt>
	      <aut>&Z;</aut>:
              <vrk>el la Unua libro de la lingvo Esperanto</vrk>.
	      <lok>Fundamenta Krestomatio, Parto 6a</lok>.
	    </fnt>.
          </ekz>
        </dif>
        <trdgrp lng="hu">
          <trd>motor</trd>,
          <trd>mozgat&oacute;</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_d;&c_v;&c_i;&c_g;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
          <trd>&c_d;&c_v;&c_i;&c_zh;&c_u;&c_shch;&c_a;&c_ja; &c_s;&c_i;&c_l;&c_a;</trd>
        </trdgrp>
      </snc>
      <ref tip="vid" cel="mov.0ilo">movilo</ref>
      <trdgrp lng="be">
        <trd>&c_m;&c_a;&c_t;&c_o;&c_r;</trd>,
        <trd>&c_r;&c_u;&c_h;&c_a;&c_v;&c_ib;&c_k;</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>motor</trd>,
        <trd>engine</trd>
      </trdgrp>
      <trd lng="fr">moteur<klr> (subst.)</klr></trd>
      <trd lng="nl">motor</trd>
    </drv>

    <drv mrk="motor.0isto">
      <kap><tld/>isto</kap>
      <snc mrk="motor.0isto.tehxnikisto">
        <dif>
          Laboristo, maristo,
          <ref tip="super" cel="tehxnik.0isto">te&hcirc;nikisto</ref>
          prizorganta <tld/>ojn.
        </dif>
        <trdgrp lng="en">
          <trd>motor-mechanic</trd>,
          <trd>machinist</trd>
        </trdgrp>
        <trd lng="ru">&c_m;&c_o;&c_t;&c_o;&c_r;&c_i;&c_s;&c_t;</trd>
      </snc>
      <snc mrk="motor.0isto.auxtisto">
        <ref tip="dif" cel="auxt.0isto">a&ubreve;tisto</ref>
        <trd lng="en">motorist</trd>
        <trd lng="hu">aut&oacute;s</trd>
        <trd lng="nl">automobilist</trd>
        <trd lng="ru">&c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;&c_i;&c_l;&c_i;&c_s;&c_t;</trd>
      </snc>
      <trd lng="be">&c_m;&c_a;&c_t;&c_a;&c_r;&c_y;&c_s;&c_t;</trd>
    </drv>

    <drv mrk="motor.eksplod0o">
      <kap>eksplod<tld/>o</kap>
      <snc>
        <dif>
          Speco de <ref tip="super" cel="motor.0o.TEK">motoro</ref>
          en kiu eksplodoj de miksa&jcirc;o de brula&jcirc;o kaj aero pu&scirc;adas 
          pi&scirc;tojn.
        </dif>
      </snc>
      <trd lng="be">&c_r;&c_u;&c_h;&c_a;&c_v;&c_ib;&c_k;
&c_u;&c_n;&c_u;&c_t;&c_r;&c_a;&c_n;&c_a;&c_g;&c_a;
&c_z;&c_g;&c_a;&c_r;&c_a;&c_n;&c_mol;&c_n;&c_ja;</trd>
      <trd lng="de">Verbrennungsmotor</trd>
      <trd lng="en">internal combustion engine</trd>
      <trd lng="fr">moteur &agrave; explosion</trd>
      <trdgrp lng="hu">
        <trd>robban&oacute;motor</trd>,
        <trd>bels&odblac; &eacute;g&eacute;s&udblac; <ind>motor</ind></trd>
      </trdgrp>
      <trd lng="ru">&c_d;&c_v;&c_i;&c_g;&c_a;&c_t;&c_je;&c_l;&c_mol;
&c_v;&c_n;&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_je;&c_g;&c_o;
&c_s;&c_g;&c_o;&c_r;&c_a;&c_n;&c_i;&c_ja;</trd>
    </drv>

    <drv mrk="motor.vapor0o">
      <kap>vapor<tld/>o</kap>
      <snc>
        <dif>
          Speco de <ref tip="super" cel="motor.0o.TEK">motoro</ref>
          kiu uzas akvan vaporon por pu&scirc;adi pi&scirc;tojn.
        </dif>
        <refgrp tip="sin">
          <ref cel="lokomo.0o">lokomobilo</ref>.
        </refgrp>
      </snc>
      <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_v;&c_y;
<ind>&c_r;&c_u;&c_h;&c_a;&c_v;&c_ib;&c_k;</ind></trd>
      <trdgrp lng="en">
        <trd>steam engine</trd>,
        <trd>external combustion engine</trd>
      </trdgrp>
      <trd lng="hu">g&odblac;zmotor</trd>  
      <trdgrp lng="ru">
        <trd>&c_p;&c_a;&c_r;&c_o;&c_v;&c_o;&c_j;
&c_d;&c_v;&c_i;&c_g;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
        <trd>&c_d;&c_v;&c_i;&c_g;&c_a;&c_t;&c_je;&c_l;&c_mol;
&c_v;&c_n;&c_je;&c_sh;&c_n;&c_je;&c_g;&c_o; &c_s;&c_g;&c_o;&c_r;&c_a;&c_n;&c_i;&c_ja;</trd>
      </trdgrp>
    </drv>
  </art>
