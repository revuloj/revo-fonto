<drv mrk="motor.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="motor.0isto.tehxnikisto">
    <dif>
      Laboristo, maristo,
      <ref tip="super" cel="tehxnik.0isto">te&hcirc;nikisto</ref>
      prizorganta <tld/>ojn:
      <ekz>
        sur la &scirc;ipo estis 21 persono<klr>[j]</klr>, pereis <tld/>isto, almena&ubreve; tri aliaj
        homoj vundi&gcirc;is
        <fnt>
          <vrk><url ref="https://eo.wikinews.org/wiki/En_Moskvo_brulegis_kaj_surfundi%C4%9Dis_motor%C5%9Dipo_%C2%ABSergej_Abramov%C2%BB">En Moskvo brulegis kaj surfundi&gcirc;is motor&scirc;ipo &#xAB;Sergej Abramov&#xBB;</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        sur la bordo hantis iaj grizaj figuroj, la <tld/>isto kaj la direktilisto
        inter&scirc;an&gcirc;is rigardojn
        <fnt>
          <aut>Ivan Efremov, trad. Jurij Finkel</aut>
          <vrk><url ref="https://www.esperanto.mv.ru/Efremov/andromedo_11.html">La nebulozo de Andromedo</url></vrk>
          <lok>&ccirc;ap 11, La insulo de Forgeso</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>motorista <klr>(t&egrave;cnic)</klr></trd>,
      <trd>maquinista</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Maschinist</trd>,
      <trd><ind>Mechaniker</ind> f&uuml;r Motoren</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>motor-mechanic</trd>,
      <trd>machinist</trd>
    </trdgrp>
    <trd lng="fa">&a_m;&a_w;&a_t;&a_w;&a_r;&f_ch;&f_y;</trd>
    <trd lng="nl">machinist</trd>
    <trd lng="ru">&c_m;&c_o;&c_t;&c_o;&c_r;&c_i;&c_s;&c_t;</trd>
    <trdgrp lng="zh">
    <trd>&#x673A;&#x68B0;&#x5E08; <pr>j&#x12B;xi&#xE8;sh&#x12B;</pr></trd>,
    <trd>&#x6A5F;&#x68B0;&#x5E2B; <pr>j&#x12B;xi&#xE8;sh&#x12B;</pr></trd>,
    <trd>&#x673A;&#x68B0;&#x5DE5;&#x4EBA; <pr>j&#x12B;xi&#xE8;g&#x14D;ngr&#xE9;n</pr></trd>,
    <trd>&#x6A5F;&#x68B0;&#x5DE5;&#x4EBA; <pr>j&#x12B;xi&#xE8;g&#x14D;ngr&#xE9;n</pr></trd>,
    <trd>&#x673A;&#x52A1;&#x5458; <pr>j&#x12B;w&#xF9;yu&#xE1;n</pr></trd>,
    <trd>&#x6A5F;&#x52D9;&#x54E1; <pr>j&#x12B;w&#xF9;yu&#xE1;n</pr></trd>
