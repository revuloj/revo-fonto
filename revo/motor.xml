<drv mrk="motor.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="motor.0isto.tehxnikisto">
    <dif>
      Laboristo, maristo,
      <ref tip="super" cel="tehxnik.0isto">te&hcirc;nikisto</ref>
      prizorganta <tld/>ojn:
      <ekz>
        sur la &scirc;ipo estis 21 persono<klr>[j]</klr>, pereis <tld/>isto, almena&ubreve; tri aliaj
        homoj vundi&gcirc;is
        <fnt>
          <vrk><url ref="https://eo.wikinews.org/wiki/En_Moskvo_brulegis_kaj_surfundi%C4%9Dis_motor%C5%9Dipo_%C2%ABSergej_Abramov%C2%BB"
              >En Moskvo brulegis kaj surfundi&gcirc;is motor&scirc;ipo «Sergej Abramov»</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        sur la bordo hantis iaj grizaj figuroj, la <tld/>isto kaj la direktilisto
        inter&scirc;an&gcirc;is rigardojn
        <fnt>
          <aut>Ivan Efremov, trad. Jurij Finkel</aut>
          <vrk><url ref="https://www.esperanto.mv.ru/Efremov/andromedo_11.html"
              >La nebulozo de Andromedo</url></vrk>
          <lok>&ccirc;ap 11, La insulo de Forgeso</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>motorista <klr>(t&egrave;cnic)</klr></trd>,
      <trd>maquinista</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Maschinist</trd>,
      <trd><ind>Mechaniker</ind> f&uuml;r Motoren</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>motor-mechanic</trd>,
      <trd>machinist</trd>
    </trdgrp>
    <trd lng="fa">&a_m;&a_w;&a_t;&a_w;&a_r;&f_ch;&f_y;</trd>
    <trd lng="nl">machinist</trd>
    <trd lng="ru">&c_m;&c_o;&c_t;&c_o;&c_r;&c_i;&c_s;&c_t;</trd>
  </snc>
  <snc mrk="motor.0isto.auxtisto">
    <ref tip="dif" cel="auxt.0isto">a&ubreve;tisto</ref>
    <trd lng="ca">conductor</trd>
    <trd lng="de">Automobilist</trd>
    <trd lng="en">motorist</trd>
    <trd lng="fa">&a_r;&a_A;&a_n;&a_n;&a_d;&a_h;</trd>
    <trd lng="hu">aut&oacute;s</trd>
    <trd lng="nl">chauffeur</trd>
    <trd lng="ru">&c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;&c_i;&c_l;&c_i;&c_s;&c_t;</trd>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_t;&c_a;&c_r;&c_y;&c_s;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>motorista</trd>,
    <trd>&rcaron;idi&ccaron; motorov&eacute;ho vozidla
    <klr>(obecn&ecaron;)</klr></trd>
  </trdgrp>
