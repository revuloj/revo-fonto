    <drv mrk="motor.0isto">
      <kap><tld/>isto</kap>
      <snc mrk="motor.0isto.tehxnikisto">
        <dif>
          Laboristo, maristo,
          <ref tip="super" cel="tehxnik.0isto">te&hcirc;nikisto</ref>
          prizorganta <tld/>ojn.
        </dif>
        <trdgrp lng="ca">
          <trd>motorista <klr>(t&egrave;cnic)</klr></trd>,
          <trd>maquinista</trd>
        </trdgrp>
        <trdgrp lng="en">
          <trd>motor-mechanic</trd>,
          <trd>machinist</trd>
        </trdgrp>
        <trd lng="fa">&a_m;&a_w;&a_t;&a_w;&a_r;&f_ch;&f_y;</trd>
        <trd lng="nl">machinist</trd>
        <trd lng="ru">&c_m;&c_o;&c_t;&c_o;&c_r;&c_i;&c_s;&c_t;</trd>
      </snc>
      <snc mrk="motor.0isto.auxtisto">
        <ref tip="dif" cel="auxt.0isto">a&ubreve;tisto</ref>
        <trd lng="ca">conductor</trd>
        <trd lng="en">motorist</trd>
        <trd lng="fa">&a_r;&a_A;&a_n;&a_n;&a_d;&a_h;</trd>
        <trd lng="hu">aut&oacute;s</trd>
        <trd lng="nl">chauffeur</trd>
        <trd lng="ru">&c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;&c_i;&c_l;&c_i;&c_s;&c_t;</trd>
      </snc>
