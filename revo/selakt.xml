<art mrk="$Id: selakt.xml,v 1.21 2022/03/27 13:09:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>selakt</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="selakt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="selakt.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Verdeta acideta flua&jcirc;o restanta el la 
      <ref cel="lakt.0o">lakto</ref> post apartigo de
      <ref cel="krem.0o">kremo</ref>
      kaj <ref cel="kaze.0o">kazeo</ref>:
      <ekz>
        la man&gcirc;o <klr>...</klr> grandparte konsistis el ka&ccirc;o kaj 
        <tld/>o
        <fnt><aut>R. L. Stevenson, trad. A. Goodheir</aut>, 
        <vrk>Forkaptita</vrk>, <lok>Kardo, 1976</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_r;&c_o;&c_v;&c_a;&c_t;&c_k;&c_a;</trd>
  <trd lng="bg">&c_s;&c_u;&c_r;&c_o;&c_v;&c_a;&c_t;&c_k;&c_a;</trd>
  <trd lng="ca">xerigot</trd>
  <trd lng="cs">syrov&aacute;tka</trd>
  <trd lng="de">Molke</trd>
  <trd lng="en">whey</trd>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_b; &f_p;&a_n;&f_y;&a_r;</trd>,
    <trd>&f_p;&a_n;&f_y;&a_r;&a_A;&a_b;</trd>
  </trdgrp>
  <trd lng="fr">petit lait</trd>
  <trd lng="hu">tejsav&oacute;</trd>
  <trd lng="it">siero di <ind>latte</ind></trd>
  <trdgrp lng="nl">
    <trd>wei</trd>,
    <trd>melkwei</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_l;&c_o;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_s;&c_y;&c_v;&c_o;&c_r;&c_o;&c_t;&c_k;&c_a;</ind></trd>,
    <trd>&c_p;&c_a;&c_h;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">srv&aacute;tka</trd>
  <trd lng="uk">(&#x43C;&#x43E;&#x43B;&#x43E;&#x447;&#x43D;&#x430;) &#x441;&#x438;&#x440;&#x43E;&#x432;&#x430;&#x442;&#x43A;&#x430;</trd>
</drv>
</art>
