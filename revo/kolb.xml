<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kolb.xml,v 1.22 2024/04/17 20:21:35 revo Exp $">
<kap>
  <rad>kolb</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kolb.0o">
  <kap><tld/>o</kap>
  <snc mrk="kolb.0o.KOMUNE">
    <uzo tip="fak">MIL</uzo>
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="super" cel="ten.0ilo.enmana">Tenilo</ref>, plej ofte
      ligna, de <ref tip="malprt" cel="fusil.0o">fusilo</ref> a&ubreve; de <ref tip="malprt" cel="pistol.0o">pistolo</ref>, kiun oni eventuale
      al&scirc;ultrigas por pafo:
      <ekz>
        per la <tld/>o de mia pafilo mi enbategis la kapon de la leono
        <fnt><bib>Munchhausen</bib>, <lok>&ccirc;apitro 1a</lok></fnt>;
      </ekz>
      <ekz>
        en policejoj oni metas la vunditojn unu sur la alian, &gcirc;is la morto batas ilin
        per <tld/>oj kaj bastonoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Vojski</nom> igis <klr>[&#8230;]</klr> per <tld/>oj renversi beston, dors&#8217; al subo
        <fnt>
          <bib>SrT</bib>
          <lok>Libro IV.</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="fust.0o.TEK">fusto<sncref/></ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_k;&c_l;&c_a;&c_d; <klr tip="amb">(&c_s;&c_t;&c_r;&c_e;&c_l;&c_mol;&c_b;&c_y;)</klr></trd>,
    <trd>&c_r;&c_u;&c_k;&c_a;&c_ja;&c_c;&c_mol; <klr tip="amb">(&c_p;&c_ib;&c_s;&c_t;&c_a;&c_l;&c_je;&c_t;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="ca">culata</trd>
  <trdgrp lng="cs">
    <trd>kolba zbran&ecaron;</trd>,
    <trd>pa&zcaron;ba</trd>
  </trdgrp>
  <trd lng="de">Kolben</trd>
  <trdgrp lng="en">
    <trd>stock<klr> (nur p.p. longa pafilo)</klr></trd>,
    <trd>butt</trd>,
    <trd>grip <klr> (nur p.p. pistolo)</klr></trd>
  </trdgrp>
  <trd lng="es">culata<klr> (por donde asir la escopeta o
  pistola)</klr></trd>
  <trd lng="fr">crosse<klr tip="ind"> (d'arme &agrave; feu)</klr></trd>
  <trdgrp lng="hu">
    <trd>puskatus</trd>,
    <trd>agy<klr tip="ind"> (l&odblac;fegyver&eacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>calcio<klr> (di armi)</klr></trd>,
    <trd>calciolo</trd>
  </trdgrp>
  <trd lng="ja">&#24202;&#23614;  <pr>&#12392;&#12371;&#12362;</pr></trd>
  <trd lng="nl">kolf<klr tip="ind"> (v.vuurwapen)</klr></trd>
  <trdgrp lng="pl">
    <trd>kolba</trd>,
    <trd>nasada</trd>
  </trdgrp>
  <trd lng="ro">pat de pu&scomma;c&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_k;&c_l;&c_a;&c_d; <klr>(&c_r;&c_u;&c_zh;&c_mol;&c_ja;)</klr></trd>,
    <trd>&c_r;&c_u;&c_k;&c_o;&c_ja;&c_t;&c_k;&c_a; <klr>(&c_p;&c_i;&c_s;&c_t;&c_o;&c_l;&c_je;&c_t;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="sk">pa&zcaron;ba</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_i;&c_k;&c_l;&c_a;&c_d; (&c_r;&c_u;&c_sh;&c_n;&c_i;&c_c;&c_ib;)</trd>,
    <trd>&c_r;&c_u;&c_k;&c_o;&c_ja;&c_t;&c_k;&c_a; (&c_p;&c_ib;&c_s;&c_t;&c_o;&c_l;&c_je;&c_t;&c_a;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#19968;&#31471;&#31895;&#30340;&#26829;&#26834; <pr>y&imacron;du&amacron;nc&umacron;deg&ugrave;nb&agrave;ng</pr></trd>,
    <trd>&#26538;&#25176; <pr>qi&amacron;ngtu&omacron;</pr></trd>
  </trdgrp>
</drv>
</art>
