<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
   <kap>
    <rad>a&ubreve;tarki</rad>/o
   </kap>
   <drv mrk="auxtarki.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">POL</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <snc>
       <dif>
          Politika sendependeco. Kp. <ref tip="vid"
cel="auxtarci.0o">a&ubreve;tarcio</ref>.
       </dif>
        <trdgrp lng="de">
          <trd>Autarkie</trd>,
          <trd>politische <ind>Unabh&auml;ngigkeit</ind></trd>
       </trdgrp>
    </snc>
</drv>

<drv mrk="auxtarki.0a">
  <kap><tld/>a</kap>
  <snc>
       <dif>
          Politike sendependa.
       </dif>
        <trdgrp lng="de">
          <trd>autark</trd>,
          <trd>politisch <ind>unabh&auml;ngig</ind></trd>
       </trdgrp>
  </snc>
</drv>

</art>

