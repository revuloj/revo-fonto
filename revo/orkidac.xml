<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: orkidac.xml,v 1.15 2024/01/30 16:09:26 revo Exp $">
<kap>
  <rad>orkidac</rad>/oj <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="orkidac.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="orkidac.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref>  de <ref tip="malprt" cel="kotile.unu0aj">unukotiledonaj</ref>
      plantoj (<trd lng="la">Orchidaceae</trd>), kun okulfrapaj, ofte
      strangformaj floroj:
      <ekz>
        multaj el niaj orkidacaj plantoj absolute bezonas
        vizitojn de noktopapilioj por depreni iliajn polena&jcirc;ojn
        kaj tiel fekundigi ilin
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 3a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="orkide.0o">orkideo</ref>
  </snc>
  <trd lng="be">&c_ja;&c_t;&c_r;&c_y;&c_sh;&c_n;&c_ib;&c_k;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_o;&c_r;&c_h;&c_i;&c_d;&c_je;&c_i;</trd>
  <trd lng="cs">vstava&ccaron;ovit&eacute; <klr>(&ccaron;ele&dcaron;)</klr></trd>
  <trdgrp lng="de">
    <trd>Orchideen</trd>,
    <trd>Knabenkrautgew&auml;chse</trd>
  </trdgrp>
  <trd lng="fa">&a_th;&a_ayn;&a_l;&a_b;&f_y;&a_A;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>orchid&eacute;es</trd>,
    <trd>orchidac&eacute;es</trd>
  </trdgrp>
  <trd lng="hu">kosborf&eacute;l&eacute;k</trd>
  <trd lng="nl">orchidee&euml;nfamilie</trd>
  <trd lng="pl">storczykowate</trd>
  <trd lng="pt">orquid&aacute;ceas</trd>
  <trd lng="ro">orhidee</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_r;&c_h;&c_i;&c_d;&c_n;&c_y;&c_je;<klr tip="ind">
(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_ja;&c_t;&c_r;&c_y;&c_sh;&c_n;&c_i;&c_k;&c_o;&c_v;&c_y;&c_je;
<klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="sk">vstava&ccaron;ovit&eacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x437;&#x43E;&#x437;&#x443;&#x43B;&#x438;&#x43D;&#x446;&#x435;&#x432;&#x456;</trd>,
    <trd>&#x43E;&#x440;&#x445;&#x456;&#x434;&#x43D;&#x456;</trd>
  </trdgrp>
  <trd lng="zh">&#xF91F; <pr>l&#xE1;n</pr></trd>
</drv>
</art>
