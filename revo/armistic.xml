<vortaro>
<art mrk="$Id: armistic.xml,v 1.11 2024/05/03 12:39:34 revo Exp $">
<kap>
  <rad>armistic</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="armistic.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc mrk="armistic.0o.MIL">
    <ref tip="dif" cel="pauxz.milit0o">militpa&ubreve;zo</ref>
    <ekz>
      oni sentis, ke ne estas ankora&ubreve; paco kun Italujo, sed sole
      tempa <tld/>o
      <fnt>
      <aut>Josef Svatopluk Machar, trad. R. Fridrich</aut>,
      <vrk><url ref="http://eo.wikibooks.org/wiki/Benedek">Benedek</url></vrk>,
      <lok>1913</lok>
      </fnt>;
    </ekz><ekz>
      minuton post paso de la tritaga <tld/>o falis la unua pafo
      <fnt>
      <bib>Salamandroj</bib>,
      <lok>libro 3a, &ccirc;apitro 7a</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="cs">p&rcaron;&iacute;m&ecaron;&rcaron;&iacute;<klr> (zastaven&iacute; v&aacute;le&ccaron;n&yacute;ch akc&iacute;)</klr></trd>
  <trd lng="en">armistice</trd>
  <trd lng="fr">armistice</trd>
  <trd lng="he">&#x5E9;&#x5C1;&#x5B0;&#x5D1;&#x5B4;&#x5D9;&#x5EA;&#x5B7;&#x5EA;
  &#x5E0;&#x5B6;&#x5E9;&#x5C1;&#x5B6;&#x5E7;</trd>
  <trd lng="it">armistizio</trd>
  <trd lng="ja">&#x4F11;&#25126;  <pr>&#x304D;&#12421;&#12358;&#x305B;&#12435;</pr></trd>
  <trd lng="pl">rozejm</trd>
  <trd lng="sk">pr&iacute;merie</trd>
  <trd lng="uk">&c_p;&c_je;&c_r;&c_je;&c_m;&c_i;&c_r;&#8217;&c_ja;</trd>
</drv>
</art>

