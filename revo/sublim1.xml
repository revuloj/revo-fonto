<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>sublim</rad>/i
</kap>
<drv mrk="sublim1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Pasigi korpon de solida stato direkte en la vaporan.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_z;&c_g;&c_a;&c_n;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_u;&c_b;&c_l;&c_ib;&c_m;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">sublimar</trd>
  <trdgrp lng="cs">
    <trd>podstupovat sublimaci</trd>,
    <trd>sublimovat</trd>
  </trdgrp>
  <trd lng="de">sublimieren</trd>
  <trd lng="fa">&a_t;&a_S;&a_ayn;&f_y;&a_d; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">sublimer</trd>
  <trd lng="hu">szublim&aacute;l</trd>
  <trd lng="nl">sublimeren</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_z;&c_g;&c_o;&c_n;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_u;&c_b;&c_l;&c_i;&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odpariť sa</trd>,
    <trd>vyprchať</trd>
  </trdgrp>
</drv>

<drv mrk="sublim1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i.
    </dif>
    <refgrp tip="sin">
      <ref cel="sublim2.0o">sublimato</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_z;&c_g;&c_o;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_b;&c_l;&c_ib;&c_m;&c_a;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
  <trd lng="ca">sublimaci&oacute;</trd>
  <trd lng="cs">sublimace</trd>
  <trd lng="de">Sublimation</trd>
  <trd lng="fa">&a_t;&a_S;&a_ayn;&f_y;&a_d;</trd>
  <trd lng="fr">sublimation</trd>
  <trdgrp lng="hu">
    <trd>szublim&aacute;l&aacute;s</trd>,
    <trd>szublim&aacute;ci&oacute;</trd>
  </trdgrp>
  <trd lng="nl">sublimatie</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_z;&c_g;&c_o;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_b;&c_l;&c_i;&c_m;&c_a;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">sublimácia</trd>
</drv>
</art>
