<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sublim1.xml,v 1.19 2023/11/25 01:31:54 revo Exp $">
<kap>
  <rad>sublim</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sublim1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sublim1.0i.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="super" cel="pas.0igi">Pasigi</ref> korpon de solida stato rekte en gasan:
      <ekz>
        por sublimi, <klr>[necesas]</klr> specifaj premoj kaj temperaturoj,
        <klr>[&#8230;]</klr> ili devas sin trovi sub la triobla punkto en la fazodiagramo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=121995">Sublimado</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="likv.0igi">likvigi</ref>,
       <ref cel="vapor.0igi">vaporigi</ref>
    </refgrp>
  </snc>
    <rim>
      &leftquot;<ref tip="hom" cel="sublim.0a">Sublima</ref>&rightquot; havas anka&ubreve; signifon spiritan.
    </rim>
  <trdgrp lng="be">
    <trd>&c_u;&c_z;&c_g;&c_a;&c_n;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_u;&c_b;&c_l;&c_ib;&c_m;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">sublimar</trd>
  <trdgrp lng="cs">
    <trd>podstupovat sublimaci</trd>,
    <trd>sublimovat</trd>
  </trdgrp>
  <trd lng="de">sublimieren</trd>
  <trd lng="fa">&a_t;&a_S;&a_ayn;&f_y;&a_d; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">sublimer</trd>
  <trd lng="hu">szublim&aacute;l</trd>
  <trd lng="ja">&#26119;&#x83EF;&#12373;&#x305B;&#x308B;  <pr>&#12375;&#12423;&#12358;&#x304B;&#12373;&#x305B;&#x308B;</pr></trd>
  <trd lng="nl">sublimeren</trd>
  <trd lng="pl">sublimowa&cacute;  <klr>(chem)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_z;&c_g;&c_o;&c_n;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_u;&c_b;&c_l;&c_i;&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odpari&tcaron; sa</trd>,
    <trd>vyprcha&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="sublim1.0o">
  <kap><tld/>o</kap>
  <snc mrk="sublim1.0o.KEM">
    <dif>
      Ago <tld/>i.
    </dif>
    <refgrp tip="vid">
      <ref cel="sublim2.0o">sublimato</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_z;&c_g;&c_o;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_b;&c_l;&c_ib;&c_m;&c_a;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
  <trd lng="ca">sublimaci&oacute;</trd>
  <trd lng="cs">sublimace</trd>
  <trd lng="de">Sublimation</trd>
  <trd lng="fa">&a_t;&a_S;&a_ayn;&f_y;&a_d;</trd>
  <trd lng="fr">sublimation</trd>
  <trdgrp lng="hu">
    <trd>szublim&aacute;l&aacute;s</trd>,
    <trd>szublim&aacute;ci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5D07;&#x9AD8;  <pr>&#12377;&#12358;&#12371;&#12358;</pr></trd>,
    <trd>&#x81F3;&#x9AD8;  <pr>&#12375;&#12371;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">sublimatie</trd>
  <trd lng="pl">sublimacja</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_z;&c_g;&c_o;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_b;&c_l;&c_i;&c_m;&c_a;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">sublim&aacute;cia</trd>
  <trdgrp lng="uk">
    <trd>&c_shch;&#x43E;-&#x43D;. &#x43F;&c_ib;&c_d;&#x43D;&c_je;&c_s;&c_je;&#x43D;&c_je;</trd>,
    <trd>&c_v;&c_je;&#x43B;&c_i;&c_ch;&#x43D;&c_je;</trd>
  </trdgrp>
</drv>
</art>
