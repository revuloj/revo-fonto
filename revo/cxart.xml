<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cxart.xml,v 1.19 2022/02/12 20:27:45 revo Exp $">
<kap>
  <rad>&ccirc;art</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="cxart.0o">
<kap><tld/>o</kap>
<snc mrk="cxart.0o.EKON">
  <uzo tip="fak">JUR</uzo>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">AVI</uzo>
  <uzo tip="fak">MAR</uzo>
  <dif>
    <ref tip="super" cel="kontra3.0o.JUR">Kontrakto</ref>
    inter la luanto kaj la luiganto de &scirc;ipo a&ubreve;
    aviadilo por la transporto de varoj a&ubreve; pasa&gcirc;eroj.
  </dif>
  <trd lng="be">&c_ch;&c_a;&c_r;&c_t;&c_e;&c_r; <klr tip="amb">(&c_k;&c_a;&c_n;&c_t;&c_r;&c_a;&c_k;&c_t;)</klr></trd>
  <trd lng="ca">x&agrave;rter</trd>
    <trdgrp lng="de">
     <trd>Befrachtungsvertrag</trd>,
     <trd>Chartervertrag</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>charte-partie</trd>,
    <trd>contrat d'affr&egrave;tement</trd></trdgrp> 
  <trdgrp lng="hu">
    <trd>haj&oacute;b&eacute;rleti szerz&odblac;d&eacute;s</trd>,
    <trd>rep&uuml;l&odblac;b&eacute;rleti
    szerz&odblac;d&eacute;s</trd>
  </trdgrp>
  <trd lng="it"><ind>contratto</ind> di noleggio<klr> ( naut. e
  aeron.)</klr>
  </trd>
  <trd lng="nl">vrachtcontract</trd>
  <trdgrp lng="pl">
    <trd>charter</trd>,
    <trd>kontrakt <ind>frachtowy</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_a;&c_r;&c_t;&c_je;&c_r;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_t;&c_r;&c_a;&c_k;&c_t; &c_n;&c_a;
    &c_a;&c_r;&c_je;&c_n;&c_d;&c_u;</trd>
  </trdgrp>
</snc>
<snc mrk="cxart.0o.JUR">
  <uzo tip="fak">HIS</uzo>
  <uzo tip="fak">JUR</uzo>
  <dif>
    Mezepoka dokumento konsentanta privilegion.
  </dif>
  <trdgrp lng="be">
    <trd>&c_g;&c_r;&c_a;&c_m;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_h;&c_a;&c_r;&c_t;&c_y;&c_ja;</trd>
  </trdgrp>
  <trd lng="de">Charta</trd>
  <trd lng="fr">charte<klr tip="ind"> (acte
  m&eacute;di&eacute;val)</klr></trd>
  <trdgrp lng="hu">
    <trd>szabadalomlev&eacute;l</trd>,
    <trd>kiv&aacute;lts&aacute;glev&eacute;l</trd>
  </trdgrp>
  <trd lng="it">carta<klr> (docum. mediev.)</klr></trd>
  <trdgrp lng="nl">
    <trd>handvest</trd>,
    <trd>charter</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_a;&c_m;&c_o;&c_t;&c_a;</trd>,
    <trd>&c_h;&c_a;&c_r;&c_t;&c_i;&c_ja;</trd>
  </trdgrp>
</snc>
<snc mrk="cxart.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Nomo de <ref tip="super" cel="kontra3.0o.JUR">kontrakto</ref>
      fiksanta &gcirc;eneralajn principojn por la rilatoj
      inter diversaj partoj de la socio a&ubreve; por diplomatiaj celoj:
      <ekz>
        la <ind><tld lit="&Ccirc;"/>o de Unui&gcirc;intaj Nacioj</ind>
        le&gcirc;igis la novan situacion rilate
        la oficialajn lingvojn en Art. 111
        <fnt>
          <bib>EeP</bib>
          <lok>Unua Parto</lok>
        </fnt>;
        <trd lng="it">Statuto delle Nazioni Unite</trd>
        <trd lng="pl">Karta Narod&oacute;w Zjednoczonych</trd>
      </ekz>
      <ekz>
        <ind>Atlantika <tld lit="&Ccirc;"/>o</ind>;
        <trd lng="it">Carta Atlantica</trd>
        <trd lng="pl">Karta Altlantycka</trd>
      </ekz>
      <ekz>
        multaj landoj ratifis la <tld/>on kontra&ubreve; infana laboro kaj ekspluatado
        <fnt>
          <bib>Monato</bib>
          <aut>Tiombiano Mardia Oumarou</aut>
          <vrk><url ref="https://www.monato.be/2005/008758.php"
              >&Ccirc;esigu la ekspluatadon!</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        la manksa lingvo estas agnoskita de la E&ubreve;ropa <tld lit="&Ccirc;"/>o pri Regionaj kaj
        Malplimultaj Lingvoj
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="https://www.monato.be/2008/009420.php"
              >Brian Stowell reviviganto de mortinta lingvo</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        la unua alineo de artikolo 21 de la <tld/>o pri fundamentaj rajtoj
        <klr>[&#8230;]</klr> malpermesas ajnan diskriminacion pro kialoj koncernantaj
        lingvon
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="https://www.monato.be/2003/008051.html"
              >Bruselo-Babelo: lingvo-kulturo en E&ubreve;ropa Unio</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pakt.0o">pakto</ref>,
      <ref cel="trakta.0o.JUR">traktato<sncref/></ref>
    </refgrp>
    <trd lng="be">&c_h;&c_a;&c_r;&c_t;&c_y;&c_ja;</trd>
    <trdgrp lng="de">
       <trd>Verfassungsurkunde</trd>,
       <trd>Charta</trd>
    </trdgrp>
    <trd lng="fr">charte<klr tip="ind"> (domaine social)</klr></trd>
    <trdgrp lng="hu">
      <trd>karta</trd>,
      <trd>bulla</trd>
    </trdgrp>
   <trdgrp lng="it">
      <trd>carta<klr> (pol.)</klr></trd>,
      <trd>statuto<klr> (pol.)</klr></trd>
    </trdgrp>
    <trd lng="pl">karta</trd>
    <trd lng="nl">charter</trd>
    <trd lng="ru">&c_h;&c_a;&c_r;&c_t;&c_i;&c_ja;</trd>
  </snc>
<trd lng="ca">carta</trd>
<trd lng="en">charter</trd>
  <trd lng="he">&shin;&#1474;&#1462;&kaf;&#1462;&resh;</trd>
</drv>

<drv mrk="cxart.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="cxart.0i.TRA">
    <dif>
      Mendi, lui veturilon kaj servon de transportado per &gcirc;i:
      <ekz>
        ni portis niajn valizojn kaj dorsosakojn
        en <tld/>itan a&ubreve;tobuson
        <fnt>
          <aut>J. Lindstedt</aut>,
          <vrk><url ref="http://lingvatereno.blogspot.fr/2014/10/orienta-taglibro-7.html">
          Orienta Taglibro (7)</url></vrk>,
          <lok>2014-08-12</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="de">chartern</trd>
  </snc>
  <trd lng="fr">affr&eacute;ter</trd>
  <trd lng="he">&lamed;&shin;&kaf;&vav;&resh;</trd>
</drv>
</art>
