<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <rad>disonanc</rad>/o
    <fnt>Z</fnt>
  </kap>
  <drv mrk="disona.0o">
    <kap><tld/>o</kap>
    <snc num="1">
      <uzo tip="fak">MUZ</uzo>
      <dif>
        Malharmonia agordo. 
      </dif>
      <trd lng="bg">&c_d;&c_i;&c_s;&c_o;&c_n;&c_a;&c_n;&c_s;</trd>
      <trdgrp lng="de">
         <trd>Dissonanz</trd>,
         <trd>Missklang</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>disszonancia</trd>,
        <trd>rossz <ind>hangz&aacute;s</ind></trd>
      </trdgrp>
      <trd lng="pl">dysonans</trd>
      <trd lng="pt">disson&acirc;ncia</trd>
    </snc>
    <snc num="2">
      <uzo tip="stl">FIG</uzo>
      <dif>
        Manko de harmonio, malakordo:
        <ekz>la plej malgrandan <tld/>on de la koloroj &scirc;i tuj
        rimarkis;</ekz>
        <ekz>profunda <tld/>o inter la karakteroj.</ekz>
      </dif>
      <trdgrp lng="de">
         <trd>Dissonanz</trd>,
         <trd>Misston</trd>
      </trdgrp>
      <trd lng="pl">dysonans</trd>
      <trd lng="pt">disson&acirc;ncia</trd>
    </snc>
    <trd lng="be">&c_d;&c_y;&c_s;&c_a;&c_n;&c_a;&c_n;&c_s;</trd>
    <trd lng="en">dissonance</trd>
    <trd lng="fr">dissonance</trd>
    <trdgrp lng="hu">
      <trd>disszonancia</trd>,
      <trd>&ouml;sszhang hi&aacute;nya</trd>
    </trdgrp>
    <trd lng="it">dissonanza</trd>
    <trd lng="ru">&c_d;&c_i;&c_s;&c_s;&c_o;&c_n;&c_a;&c_n;&c_s;</trd>
  </drv>
</art>
