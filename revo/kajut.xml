<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>kajut</rad>/o
</kap>
<drv mrk="kajut.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">MAR</uzo>
   <snc>
     <dif>
       Sur&scirc;ipa &ccirc;ambreto, por &scirc;ipanoj a&ubreve; 
pasa&gcirc;eroj.
     </dif>
     <refgrp tip="vid">
       <ref cel="bud.0o">budo</ref>.
     </refgrp>
   </snc>
   <trd lng="be">&c_k;&c_a;&c_ju;&c_t;&c_a;</trd>
   <trdgrp lng="de">
     <trd>Kaj&uuml;te</trd>,
     <trd>Kabine</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>cabin</trd>,
     <trd>saloon</trd>
   </trdgrp>
   <trdgrp lng="fi">
     <trd>hytti</trd>,
     <trd>kajuutta</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>cabine<klr tip="ind"> (de bateau)</klr></trd>,
     <trd>chambre<klr tip="ind"> (cabine)</klr></trd>
   </trdgrp>
   <trd lng="hu">kaj&uuml;t</trd>
   <trd lng="nl">kajuit</trd>
   <trd lng="pt">camarote</trd>
   <trd lng="ru">&c_k;&c_a;&c_ju;&c_t;&c_a;</trd>
   <trdgrp lng="sv">
     <trd>hytt</trd>,
     <trd>kajuta</trd>
   </trdgrp>
</drv>
</art>
