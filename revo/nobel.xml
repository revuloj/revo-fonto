<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nobel.xml,v 1.28 2022/09/26 14:06:55 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>nobel</rad>/o
</kap>
<drv mrk="nobel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="nobel.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Homo, kiu pro familia deveno a&ubreve; pro favoro de regnestro
      ricevis apartajn privilegiojn kaj almena&ubreve; honoran
      titolon, kiu distingigas lin de ceteraj regnanoj:
      <ekz>
        nur &ccirc;irka&ubreve; la jaro 460 la <tld/>oj en A&ubreve;vernjo ekkutimis
        paroli
        latine
        <fnt>
        <aut>Andr&eacute; Cherpillod</aut>,
        <vrk>La galla, &ccirc;u malaperinta?</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 1998a, numero 6a, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="aristo.0o">aristokrato</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_v;&c_a;&c_r;&c_a;&c_n;i&c_n;</trd>,
    <trd>&c_sh;&c_l;&c_ja;&c_h;&c_c;i&c_c;</trd>,
    <trd>&c_z;&c_n;&c_a;&c_t;&c_n;&c_y; &c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;</trd>
  </trdgrp>
  <trd lng="cs">&scaron;lechtic</trd>
  <trdgrp lng="de">
    <trd>Adliger</trd>,
    <trd>Edelmann</trd>
  </trdgrp>
  <trd lng="el">&epsilon;&ypsilon;&gamma;&epsilon;&ny;&eta_ton;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>noble</trd>,
    <trd>nobleman</trd>    
  </trdgrp>
  <trdgrp lng="es">
    <trd>noble</trd>,
    <trd>arist&oacute;crata</trd>    
  </trdgrp>
  <trdgrp lng="fr">
    <trd>aristocrate<klr> (subst.)</klr></trd>,
    <trd>noble<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="hu">nemesember</trd>
  <trd lng="nl">edelman</trd>
  <trd lng="pl">szlachcic</trd>
  <trdgrp lng="pt">
    <trd>nobre <klr>(subst.)</klr></trd>,
    <trd>aristocrata <klr>(subst.)</klr></trd>,
    <trd>fidalgo <klr>(subst.)</klr></trd>    
  </trdgrp>
  <trd lng="sl">plemi&ccaron;</trd>
  <trd lng="ru">&c_d;&c_v;&c_o;&c_r;&c_ja;&c_n;&c_i;&c_n;</trd>
  <trd lng="sk">&scaron;&lcaron;achtic</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_v;&#x43E;&c_r;&#x44F;&#x43D;&c_i;&#x43D;</trd>,
    <trd>&c_sh;&#x43B;&#x44F;&c_h;&c_t;&c_i;&c_ch;</trd>
  </trdgrp>
</drv>

<drv mrk="nobel.0a">
  <kap><tld/>a</kap>
  <snc mrk="nobel.0a.POL">
    <dif>
      Rilata al <tld/>oj, al ties aparta&jcirc;oj, vivostilo, ecoj:
      <ekz>
        li ricevis la britan <tld/>an titolon &leftquot;Sir&rightquot;
        (kavaliro)
        <fnt>
        <aut>Evgeni Georgiev</aut>,
        <vrk>Sir Edmund Hillary</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2000a, numero 10a, p. 18a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_v;&c_a;&c_r;&c_a;&c_n;&c_s;&c_k;&c_ib;</trd>,
    <trd>&c_sh;&c_l;&c_ja;&c_h;&c_je;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_sh;&c_l;&c_ja;&c_h;&c_je;&c_ch;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>urozen&yacute;</trd>,
    <trd>&scaron;lechtick&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>adelig</trd>,
    <trd>adlig</trd>,
    <trd>blaubl&uuml;tig</trd>,
    <trd>adligen <ind>Gebl&uuml;ts</ind></trd>,
    <trd>von edler <ind>Herkunft</ind></trd>
  </trdgrp>
  <trd lng="en">noble</trd>
  <trdgrp lng="es">
    <trd>noble</trd>,
    <trd>aristocr&aacute;tico, -a</trd>    
  </trdgrp>
  <trdgrp lng="fr">
    <trd>aristocrate<klr> (adj.)</klr></trd>,
    <trd>noble<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="hu">nemes</trd>
  <trd lng="nl">adel-</trd>
  <trd lng="pl">szlachecki</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_v;&c_o;&c_r;&c_ja;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_a;&c_r;&c_i;&c_s;&c_t;&c_o;&c_k;&c_r;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_r;&c_o;&c_d;&c_n;&c_y;&c_j;<klr tip="amb">
    (&c_d;&c_v;&c_o;&c_r;&c_ja;&c_n;&c_s;&c_k;&c_i;&c_j;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>urodzen&yacute;</trd>,
    <trd>&scaron;&lcaron;achtick&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_v;&#x43E;&c_r;&#x44F;&#x43D;&c_s;&#x44C;&#x43A;&c_i;&c_j;</trd>,
    <trd>&c_sh;&#x43B;&#x44F;&c_h;&c_je;&c_t;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_sh;&#x43B;&#x44F;&c_h;&c_je;&c_t;&c_s;&#x44C;&#x43A;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="nobel.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="nobel.0eco.POL">
    <dif>
      Stato de <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_v;&c_a;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_a; <klr tip="amb">(&c_s;&c_t;&c_a;&c_n;)</klr></trd>,
    <trd>&c_sh;&c_l;&c_ja;&c_h;&c_je;&c_c;&c_t;&c_v;&c_a; <klr tip="amb">(&c_s;&c_t;&c_a;&c_n;)</klr></trd>
  </trdgrp>
  <trd lng="el">&epsilon;&ypsilon;&gamma;&epsilon_ton;&ny;&epsilon;&jota;&alfa;</trd>
  <trdgrp lng="de">
    <trd>Adel</trd>,
    <trd>edle <ind>Gesinnung</ind></trd>
  </trdgrp>
  <trd lng="en">nobility</trd>
  <trdgrp lng="fr">
    <trd>noblesse<klr> (statut)</klr></trd>
  </trdgrp>
  <trd lng="hu">nemess&eacute;g<klr tip="ind">
  (privil&eacute;gium)</klr></trd>
  <trd lng="nl">adel</trd>
  <trdgrp lng="es">
    <trd>nobleza</trd>,
    <trd>aristocracia</trd>    
  </trdgrp>
  <trd lng="pl">szlachetno&sacute;&cacute;</trd>
  <trd lng="pt">nobreza</trd>
  <trd lng="ru">&c_d;&c_v;&c_o;&c_r;&c_ja;&c_n;&c_s;&c_t;&c_v;&c_o; <klr tip="amb">(&c_t;&c_i;&c_t;&c_u;&c_l;)</klr></trd>
</drv>

<drv mrk="nobel.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="nobel.0aro.POL">
    <dif>
      Tuta klaso de la <tld/>oj:
      <ekz>
        En Japanio ne ekzistas <tld/>aro<fnt><aut>Igarasi Takeo</aut>,
        <vrk>Tronanta problemo</vrk>, <bib>Monato</bib>, <lok>jaro
        2002a,
        numero 2a, p. 14a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="aristo1.0o.nobelaro">aristokratio</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_v;&c_a;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_a; <klr tip="amb">(&c_s;&c_a;&c_c;. &c_g;&c_r;&c_u;&c_p;&c_a;)</klr></trd>,
    <trd>&c_sh;&c_l;&c_ja;&c_h;&c_je;&c_c;&c_t;&c_v;&c_a; <klr tip="amb">(&c_s;&c_a;&c_c;. &c_g;&c_r;&c_u;&c_p;&c_a;)</klr></trd>,
    <trd>&c_sh;&c_l;&c_ja;&c_h;&c_t;&c_a;</trd>,
    <trd>&c_a;&c_r;&c_y;&c_s;&c_t;&c_a;&c_k;&c_r;&c_a;&c_t;&c_y;&c_ja;</trd>
  </trdgrp>
  <trd lng="cs">&scaron;lechta</trd>
  <trdgrp lng="de">
    <trd>Adelsstand</trd>,
    <trd><klr>die </klr>Adligen</trd>
  </trdgrp>
  <trd lng="el">&omikron;&jota; &epsilon;&ypsilon;&gamma;&epsilon;&ny;&epsilon;&jota_ton;&sigma_fina;</trd>
  <trd lng="en">nobility</trd>
  <trdgrp lng="es">
    <trd>la nobleza</trd>,
    <trd>los nobles</trd>,
    <trd>la aristocracia</trd>    
  </trdgrp>
  <trdgrp lng="fr">
    <trd>aristocratie<klr> (groupe social)</klr></trd>,
    <trd>noblesse<klr> (groupe social)</klr></trd>
  </trdgrp>
  <trd lng="hu">nemess&eacute;g<klr tip="ind"> (oszt&aacute;ly)</klr></trd>
  <trd lng="nl">adel</trd>
  <trd lng="pl">szlachta</trd>
  <trd lng="pt">nobreza</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_v;&c_o;&c_r;&c_ja;&c_n;&c_s;&c_t;&c_v;&c_o; <klr tip="amb">(&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>,
    <trd>&c_a;&c_r;&c_i;&c_s;&c_t;&c_o;&c_k;&c_r;&c_a;&c_t;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">&scaron;&lcaron;achta</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_v;&#x43E;&c_r;&#x44F;&#x43D;&c_s;&c_t;&c_v;&#x43E;</trd>,
    <trd>&c_sh;&#x43B;&#x44F;&c_h;&c_t;&c_a;</trd>,
    <trd>&c_sh;&#x43B;&#x44F;&c_h;&c_je;&c_t;&c_s;&c_t;&c_v;&#x43E;</trd>
  </trdgrp>
</drv>
</art>
