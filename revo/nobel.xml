<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>nobel</rad>/o
</kap>
<drv mrk="nobel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Homo, kiu pro sia familia deveno a&ubreve; pro favoro de regnestro
      ricevis apartajn privilegiojn kaj kiu almena&ubreve; posedas honoran
      titolon, kiu distingigas lin de la ceteraj regnanoj.
    </dif>
    <refgrp tip="sin">
      <ref cel="aristo.0o">aristokrato</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Adliger</trd>,
    <trd>Edelmann</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>aristocrate</trd>,
    <trd>noble</trd>
  </trdgrp>
  <trd lng="hu">nemes<klr tip="ind"> (rang)</klr></trd>
  <trd lng="nl">edelman</trd>
  <trd lng="ru">&c_d;&c_v;&c_o;&c_r;&c_ja;&c_n;&c_i;&c_n;</trd>
</drv>

<drv mrk="nobel.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Stato de <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>noblesse<klr> (titre)</klr></trd>
  </trdgrp>
  <trd lng="hu">nemess&eacute;g<klr tip="ind">
  (privil&eacute;gium)</klr></trd>
  <trd lng="nl">adel</trd>
  <trd lng="ru">&c_d;&c_v;&c_o;&c_r;&c_ja;&c_n;&c_s;&c_t;&c_v;&c_o; <klr tip="amb">(&c_t;&c_i;&c_t;&c_u;&c_l;)</klr></trd>
</drv>

<drv mrk="nobel.0aro">
  <kap><tld/>aro</kap>
  <snc>
    <dif>
      Tuta klaso de la <tld/>oj.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>artistocratie</trd>
    <trd>noblesse<klr> (groupe social)</klr></trd>
  </trdgrp>
  <trd lng="hu">nemess&eacute;g<klr tip="ind"> (oszt&aacute;ly)</klr></trd>
  <trd lng="nl">adel</trd>
  <trd lng="ru">&c_d;&c_v;&c_o;&c_r;&c_ja;&c_n;&c_s;&c_t;&c_v;&c_o; <klr tip="amb">(&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>
</drv>
</art>
