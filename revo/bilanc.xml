<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
  <ofc>1</ofc>
  <rad>bilanc</rad>/o
  </kap>
  <drv mrk="bilanc.0o">
    <kap><tld/>o</kap>
    <snc>
      <uzo tip="fak">EKON</uzo>
      <dif>
        Tabela kalkulo, la&ubreve; aktivo k pasivo, de la financa stato
        de firmo,
        kliento, asocio, &scirc;tato, k.s.
      </dif>
      <refgrp tip="vid">
        <ref cel="invent1.0o">inventaro</ref>,
        <ref cel="sald.0o">saldo</ref>.
      </refgrp>
      <trdgrp lng="en">
         <trd>balance sheet</trd>,
         <trd>financial statement</trd>,
         <trd>annual financial statement</trd>,
         <trd>annual accounts<klr> (usone)</klr></trd>,
         <trd>statement of conditions</trd>,
         <trd>balance</trd></trdgrp>
      <trd lng="pl">bilans</trd>
      <trd lng="ru">&c_b;&c_a;&c_l;&c_a;&c_n;&c_s;</trd>
    </snc>
    <snc>
      <uzo tip="stl">FIG</uzo>
      <dif>
        Raporto pri la pozitivaj kaj negativaj rezultoj de iu ago.
      </dif>
      <trdgrp lng="de">
         <trd>Aufstellung</trd>,
         <trd>Abrechnung</trd>
      </trdgrp>
      <trd lng="pl">bilans</trd>
    </snc>
    <trd lng="be">&c_b;&c_a;&c_l;&c_ja;&c_n;&c_s;</trd>
    <trd lng="de">Bilanz</trd>
    <trd lng="es">balance</trd>  
    <trd lng="en">balance</trd>  
    <trd lng="fr">bilan</trd>
    <trd lng="hu">m&eacute;rleg</trd>
    <trd lng="sk">bilancia</trd>
  </drv>
  
  <drv mrk="bilanc.0i">
    <kap><tld/>i</kap>
    <gra><vspec>tr</vspec></gra>
    <uzo tip="fak">EKON</uzo>
    <snc>
      <dif>
        Starigi <tld/>on: <ekz><tld/>i konton.
        </ekz>
      </dif>
      <trd lng="be">&c_p;&c_a;&c_d;&c_v;&c_je;&c_s;&c_mol;&c_c;&c_ib;
      &c_b;&c_a;&c_l;&c_ja;&c_n;&c_s;</trd>
      <trd lng="de">bilanzieren</trd>
      <trdgrp lng="en">
        <trd>reconcile<klr> (an account)</klr></trd>,
        <trd>balance</trd></trdgrp>
      <trdgrp lng="fr">
        <trd>faire le <ind>bilan</ind> (de)</trd>,
        <trd>&eacute;quilibrer <klr tip="ind">(un
        compte)</klr></trd></trdgrp>
      <trd lng="hu">m&eacute;rleget k&eacute;sz&iacute;t</trd>
      <trd lng="pl">robi&cacute; <ind>bilans</ind></trd>
      <trd lng="ru">&c_p;&c_o;&c_d;&c_v;&c_je;&c_s;&c_t;&c_i;
      &c_b;&c_a;&c_l;&c_a;&c_n;&c_s;</trd>
      <trd lng="sk">bilancova&tcaron;</trd>
    </snc>
  </drv>
</art>
