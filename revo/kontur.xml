<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kontur</rad>/o
</kap>
<drv mrk="kontur.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Linio, kiu limigas ekstere objekton:
      <ekz>
        la <tld/>o de vazo, mamo;
      </ekz>
      <ekz>
        verda makulo kun klaraj <tld/>oj<fnt>B</fnt>;
      </ekz>
      <ekz>
        oni vidis klare la <tld/>ojn de &ccirc;iuj montoj;
      </ekz>
      <ekz>
        fari <tld/>ojn per grifelo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tra la nebulo oni <tld/>e vidas arbojn.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="siluet.0o">silueto</ref>,
      <ref cel="skiz.0o">skizo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="en">
    <trd>Umriss</trd>,
    <trd>Umrisslinie</trd>,
    <trd>Kontur</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>contour</trd>,
    <trd>outline</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_n;&c_t;&c_u;&c_r;</trd>,
    <trd>&c_o;&c_ch;&c_je;&c_r;&c_t;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_a;&c_b;&c_r;&c_i;&c_s;</trd>
  </trdgrp>
  <trd lng="sv">kontur</trd>
</drv>

<drv mrk="kontur.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Desegni la <tld/>on de:
      <ekz>
        la tran&ccirc;e <tld/>ita arba muro;
      </ekz>
      <ekz>
        en la malproksimo <tld/>i&gcirc;is la turpinto de pre&gcirc;ejo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>umrei&szlig;en</trd>,
    <trd>in Umriss zeichnen</trd>,
    <trd>die Konturen zeigen</trd>
  </trdgrp>
</drv>
</art>
