<art mrk="$Id: prior.xml,v 1.19 2018/12/07 19:10:15 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>prior</rad>/o
</kap>

<drv mrk="prior.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">KAT</uzo>
    <dif>
      <ref tip="super" cel="monahx.0ejestro">Mona&hcirc;ejestro</ref>
      en iuj romkatolikaj ordenoj; titolo responda al
      <ref cel="abat.0o">abato</ref> &ccirc;e la benediktanoj:
      <ekz>
        sankta <nom>Guigo</nom> <klr>[&#8230;]</klr> kiel <tld/>o gvidis la Grandan Kartuzion inter la
        jaroj 1109 kaj 1136
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=46579"
              >Kartuzianoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_p;&c_r;&c_i;&c_o;&c_r;</trd>
  <trd lng="ca">prior</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;evor</trd>,
    <trd>prior</trd>,
    <trd>p&rcaron;evor</trd>
  </trdgrp>
  <trd lng="da">prior</trd>
  <trd lng="de">Prior</trd>
  <trd lng="en">prior<klr tip="amb"> (ecclesiastical
      title)</klr></trd>
  <trd lng="es">prior</trd>
  <trd lng="fr">prieur</trd>
  <trdgrp lng="hu">
    <trd>h&aacute;zf&odblac;n&ouml;k</trd>,
    <trd>prior</trd>,
    <trd>perjel</trd>
  </trdgrp>
  <trd lng="it">priore</trd>
  <trd lng="nl">prior</trd>
  <trd lng="pl">przeor</trd>
  <trd lng="pt">prior <klr>(&ocirc;)</klr></trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_o;&c_r;</trd>
  <trd lng="sk">prior</trd>
</drv>
