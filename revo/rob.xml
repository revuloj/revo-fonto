        <trd>japon</trd>
      </trdgrp>
      <trdgrp lng="ja">
        <trd>&#x30C9;&#x30EC;&#x30B9;</trd>,
        <trd>&#x30EF;&#x30F3;&#x30D4;&#x30FC;&#x30B9;</trd>
