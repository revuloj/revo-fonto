<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aktin.xml,v 1.6 2022/04/25 15:10:59 revo Exp $">
<kap>
  <rad>aktin</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="aktin.0o">
  <kap><tld/>o</kap>
  <snc mrk="aktin.0o.FILZ">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      <ref tip="super" cel="protein.0o.FIZL">Proteino</ref>
      sufi&ccirc;e abunde trovebla en &ccirc;iuj e&ubreve;kariotaj &ccirc;eloj,
      interalie rolanta kune kun
      <ref tip="vid" cel="miozin.0o.FIZL">miozino</ref>
      en muskolaj movoj:
      <ekz>
        miloj da molekuloj de <tld/>o kunmunti&gcirc;as en 
        filamento de <tld/>o
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Proteina_strukturo">
          Proteina strukturo</url>, 2013-04-22</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_t;&c_y;&c_n;</trd>
  <trd lng="cs">aktin</trd>
  <trd lng="en">actin</trd>
  <trd lng="fr">actine</trd>
  <trd lng="sk">akt&#xED;n</trd>
  <trd lng="uk">&c_a;&#x43A;&c_t;&c_i;&#x43D;</trd>
</drv>
</art>

