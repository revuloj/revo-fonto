    <trd>&c_s;&c_t;&c_r;&c_o;&c_m;&c_y;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_o;&c_m;&c_k;&c_ib;</trd>,
    <trd>&c_a;&c_b;&c_r;&c_y;&c_v;&c_ib;&c_s;&c_t;&c_y;</trd>
  </trdgrp>
  <trd lng="nl">klimmend</trd>
  <trd lng="sk">strm&yacute;</trd>
  <trd lng="cs">strm&yacute;</trd>
  <trd lng="pl">stromy</trd>
</drv>

<drv mrk="grimp.0ulo">
  <kap><tld/>ulo</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
