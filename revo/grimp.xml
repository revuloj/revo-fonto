  <trd lng="am">&#4632;&#x12CD;&#4899;&#4725;</trd>
  <trd lng="ar">&#x62A;&a_s;&a_l;&a_q;</trd>
  <trd lng="az">qalxma</trd>
  <trdgrp lng="be">
