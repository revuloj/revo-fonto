<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vitac.xml,v 1.11 2022/05/19 07:31:24 revo Exp $">
<kap>
  <rad>vitac</rad>/oj
</kap>
<drv mrk="vitac.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref> de <ref
      tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Vitaceae</trd>), al kiu apartenas la <ref tip="prt"
      cel="vit.0o">vito</ref>:
      <ekz>
        beroj rikoltataj de vito el la familio <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=27290">Vinbero</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_n;&c_a;&c_g;&c_r;&c_a;&c_d;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  <trd lng="ca">vit&agrave;cies</trd>
  <trd lng="de">Rebengew&auml;chse</trd>
  <trd lng="hu">sz&odblac;l&odblac;f&eacute;l&eacute;k</trd>
  <trd lng="nl">wijnstokfamilie</trd>
  <trd lng="pt">vit&aacute;ceas</trd>
  <trd lng="ru">&c_v;&c_i;&c_n;&c_o;&c_g;&c_r;&c_a;&c_d;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&c_v;&c_i;&#x43D;&#x43E;&c_g;&c_r;&c_a;&c_d;&#x43D;&c_ib;</trd>
</drv>
</art>
