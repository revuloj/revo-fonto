<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mnemoteknik.xml,v 1.5 2019/03/11 20:10:22 revo Exp $">
<kap>
  <rad>mnemoteknik</rad>/o,
    <var><kap><rad>menmote&hcirc;nik</rad>/o</kap></var>
</kap>
<drv mrk="mnemoteknik.0o">
  <kap><tld/>o,
    <var><kap>mnemote&hcirc;niko</kap></var>
  </kap>
  <snc>
    <dif>
      <ref tip="dif" cel="art.memor0o.TEK">Memorarto</ref>:
      <ekz>
        d-ro Nicolas emfazis kiel avanta&gcirc;o de sia sistemo fondita sur aprioraj
        principoj, ke &gcirc;i estas konstruita la&ubreve; firma studo pri la le&gcirc;oj de
        mnemote&hcirc;niko kaj sekve estas aparte facile memorebla
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=37892"
              >Spokil</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[pri]</klr> Polimnio <klr>[&#8230;]</klr> tradicio diras ke &scirc;i helpas mnemote&hcirc;nikon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=101712"
              >Polimnio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="mnemon.0o">mnemoniko</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_n;&c_e;&c_m;&c_o;&c_n;&c_ib;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_n;&c_e;&c_m;&c_a;&c_t;&c_e;&c_h;&c_n;&c_ib;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">mnemotecnia</trd>
  <trdgrp lng="de">
    <trd>Mnemonik</trd>,
    <trd>Mnemotechnik</trd>
  </trdgrp>
  <trd lng="es">mnemot&eacute;cnica</trd>
  <trd lng="fr">mn&eacute;motechnie</trd>
  <trdgrp lng="hu">
    <trd>mnemotechnika</trd>,
    <trd>mnemonika</trd>
  </trdgrp>  
  <trdgrp lng="nl">
    <trd>geheugenkunst</trd>,
    <trd>mnemotechniek</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>mnem&ocirc;nica</trd>,
   <trd>mnemot&eacute;cnica</trd>,
   <trd>mnemotecnia</trd>   
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_n;&c_je;&c_m;&c_o;&c_n;&c_i;&c_k;&c_a;</trd>,
 
<trd>&c_m;&c_n;&c_je;&c_m;&c_o;&c_t;&c_je;&c_h;&c_n;&c_i;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">mnemotechnika</trd>
</drv>
</art>

