<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mnemoteknik.xml,v 1.8 2024/01/26 19:05:52 revo Exp $">
<kap>
  <rad>mnemoteknik</rad>/o,
    <var><kap><rad var="&hcirc;">menmote&hcirc;nik</rad>/o</kap></var>
</kap>
<drv mrk="mnemoteknik.0o">
  <kap><tld/>o,
    <var><kap><tld var="&hcirc;"/>o</kap></var>
  </kap>
  <snc>
    <dif>
      <ref tip="dif" cel="art.memor0o.TEK">Memorarto</ref>:
      <ekz>
        d-ro Nicolas emfazis kiel avanta&gcirc;o de sia sistemo fondita sur aprioraj
        principoj, ke &gcirc;i estas konstruita la&ubreve; firma studo pri la le&gcirc;oj de
        <tld var="&hcirc;"/>o kaj sekve estas aparte facile memorebla
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=37892">Spokil</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[pri]</klr> Polimnio <klr>[&#x2026;]</klr> tradicio diras ke &scirc;i helpas <tld var="&hcirc;"/>on
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=101712">Polimnio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="mnemon.0o">mnemoniko</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_n;&c_e;&c_m;&c_o;&c_n;&c_ib;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_n;&c_e;&c_m;&c_a;&c_t;&c_e;&c_h;&c_n;&c_ib;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">mnemotecnia</trd>
  <trdgrp lng="de">
    <trd>Mnemonik</trd>,
    <trd>Mnemotechnik</trd>
  </trdgrp>
  <trd lng="es">mnemot&eacute;cnica</trd>
  <trd lng="fr">mn&eacute;motechnie</trd>
  <trdgrp lng="hu">
    <trd>mnemotechnika</trd>,
    <trd>mnemonika</trd>
  </trdgrp>  
  <trdgrp lng="nl">
    <trd>geheugenkunst</trd>,
    <trd>mnemotechniek</trd>
  </trdgrp>
  <trd lng="pl">mnemotechnika</trd>
  <trdgrp lng="pt">
   <trd>mnem&ocirc;nica</trd>,
   <trd>mnemot&eacute;cnica</trd>,
   <trd>mnemotecnia</trd>   
  </trdgrp>
  <trd lng="ro">mnemotehnic&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_n;&c_je;&c_m;&c_o;&c_n;&c_i;&c_k;&c_a;</trd>, 
    <trd>&c_m;&c_n;&c_je;&c_m;&c_o;&c_t;&c_je;&c_h;&c_n;&c_i;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">mnemotechnika</trd>
  <trdgrp lng="zh">
    <trd>&#x8A18;&#x61B6;&#x8853; <pr>j&#xEC;y&#xEC;sh&#xF9;</pr></trd>,
    <trd>&#x8A18;&#x61B6;&#x672E; <pr>j&#xEC;y&#xEC;sh&#xF9;</pr></trd>,
    <trd>&#x8BB0;&#x5FC6;&#x672F; <pr>j&#xEC;y&#xEC;sh&#xF9;</pr></trd>,
    <trd>&#x52A9;&#x5FC6;&#x7801; <pr>zh&#xF9;y&#xEC;m&#x1CE;</pr></trd>,
    <trd>&#x52A9;&#x61B6;&#x78BC; <pr>zh&#xF9;y&#xEC;m&#x1CE;</pr></trd>
  </trdgrp>
</drv>
</art>

