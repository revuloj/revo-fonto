<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mirtac.xml,v 1.18 2022/03/23 14:02:26 revo Exp $">
<kap>
  <rad>mirtac</rad>/oj
</kap>
<drv mrk="mirtac.0oj">
  <kap><tld/>oj</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>
      arboj kaj arbedoj (<trd lng="la">Myrtaceae</trd>):
      <ekz>
        montaraj humidaj arbaroj trovi&gcirc;as <klr>[&#x2026;]</klr> tra la montaroj
        
        kaj altebena&jcirc;oj de suda Brazilo, kaj gastigas la
        
        ara&ubreve;kariojn
        kaj &ccirc;iamverdajn arbojn el la familioj de la&ubreve;racoj kaj <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=307292">Atlantikaj
          arbaroj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="kariof.0ujo">kariofilujo</ref>,
      <ref cel="euxkal.0o">e&ubreve;kalipto</ref>,
      <ref cel="piment.0o.BOT">pimento<sncref/></ref>,
       <ref cel="gujav.0arbo.BOT">gujavujo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_r;&c_t;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_m;&c_i;&c_r;&c_t;&c_o;&c_v;&c_i;</trd>
  <trd lng="ca">mirt&agrave;cies</trd>
  <trd lng="cs">myrtovit&eacute; <klr>(&ccaron;ele&dcaron;)</klr></trd>
  <trd lng="de">Myrtengew&auml;chse</trd>
  <trd lng="fa">&a_t;&f_y;&a_r;&a_h;&a_hamza_sure; <ind>&a_m;&a_w;&a_r;&a_sukun;&a_d;</ind></trd>
  <trd lng="fr">Myrtac&eacute;es</trd>
  <trd lng="hu">mirtuszf&eacute;l&eacute;k</trd>
  <trd lng="nl">mirtefamilie</trd>
  <trd lng="pt">mirt&aacute;ceas</trd>
  <trd lng="ru">&c_m;&c_i;&c_r;&c_t;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">myrtovit&eacute;</trd>
  <trd lng="uk">&#x43C;&#x438;&#x440;&#x442;&#x43E;&#x432;&#x456;</trd>
</drv>
</art>
