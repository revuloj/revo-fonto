<kap>
  <rad>niel</rad>/o
</kap>

<drv mrk="niel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Gravurita <ref tip="super" cel="desegn.0o.desegnajxo">desegno</ref>,
      kies entajlojn oni plenigas per nigra emajlo:
      <ekz>
        la plej fruaj kuprogravura&jcirc;oj montras en sia bildstrukturo klarajn similecojn al
        <tld/>o, tekniko por ornamado de altvaloraj metalobjektoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=288883">Kuprogravurado</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_e;&c_r;&c_n;&c_mol; <klr tip="amb">(&c_n;&c_a;&c_p;&c_r;., &c_n;&c_a;
  &c_s;&c_r;&c_e;&c_b;&c_r;&c_y;)</klr></trd>
  <trd lng="cs">&ccaron;ern&yacute; smalt</trd>
  <trd lng="de">Niello</trd>
  <trd lng="en">niello</trd>
  <trd lng="hu">niello</trd>
  <trdgrp lng="ja">
    <trd>&#x30CB;&#x30A8;&#x30ED;</trd>,
    <trd>&#x9ED2;&#x91D1;  <pr>&#x304F;&#x308D;&#x304C;&#x306D;</pr></trd>,
    <trd>&#x30CB;&#x30A8;&#x30ED;&#x8C61;&#x773C;&#x7D30;&#x5DE5;  <pr>&#x30CB;&#x30A8;&#x30ED;&#x305E;&#x3046;&#x304C;&#x3093;&#x3055;&#x3044;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">ni&euml;llo</trd>
  <trd lng="pl">niello  <klr>(tech. zdobnicza)</klr></trd>
  <trdgrp lng="pt">
    <trd>nigelo</trd>,
    <trd>nielo</trd>,
    <trd>niela</trd>
  </trdgrp>
  <trd lng="ro">niello</trd>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_n;&c_mol; <klr tip="amb">(&c_n;&c_a; &c_ju;&c_v;&c_je;&c_l;&c_i;&c_r;&c_n;&c_y;&c_h;
  &c_i;&c_z;&c_d;&c_je;&c_l;&c_i;&c_ja;&c_h;)</klr></trd>
  <trd lng="sk">&ccaron;ierny smalt</trd>
  <trdgrp lng="uk">
    <trd>&#x447;&#x435;&#x440;&#x43D;&#x44C;</trd>,
    <trd>&#x447;&#x43E;&#x440;&#x43D;&#x430; &#x435;&#x43C;&#x430;&#x43B;&#x44C; (&#x43D;&#x430; &#x44E;&#x432;&#x435;&#x43B;&#x456;&#x440;&#x43D;&#x438;&#x445; &#x432;&#x438;&#x440;&#x43E;&#x431;&#x430;&#x445;)</trd>
  </trdgrp>
</drv>

