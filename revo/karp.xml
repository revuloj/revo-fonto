<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>karp</rad>/o
</kap>
<drv mrk="karp.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de
      <ref tip="super" cel="ciprin.0o">ciprino</ref>
      (<trd lng="la">Cyprinus carpio</trd>),
      bongusta riverfi&scirc;o.
    </dif>
  </snc>
  <trd lng="de">Karpfen</trd>
  <trd lng="en">carp</trd>
  <trd lng="fr">carpe</trd>
  <trd lng="nl">karper</trd>
  <trd lng="pl">karp</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_r;&c_p;</trd>,
    <trd>&c_s;&c_a;&c_z;&c_a;&c_n;</trd>
  </trdgrp>
</drv>
</art>
