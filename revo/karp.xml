<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: karp.xml,v 1.25 2023/12/03 08:00:24 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>karp</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="karp.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="karp.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Bongusta <ref tip="lst" cel="fisx.0o" lst="voko:fi&scirc;oj">fi&scirc;o</ref> 
      de E&ubreve;ropo kaj Azio, vivanta en malrapidaj 
      riveroj kaj lagoj; 
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:zoologiaj_specioj">specio</ref> de 
      <ref tip="super" cel="ciprin.0o">ciprino</ref>
      (<trd lng="la">Cyprinus carpio</trd>):
      <ekz>
        kiam la &ccirc;asantoj sin revestis kaj de&scirc;iris de si hirudojn, la 
        <tld/>o jam estas salumita kaj fritita super karbo
        <fnt>
          <aut>J. Kozarac, trad. D. Klobu&ccaron;ar</aut>,
          <vrk><url ref="http://public.carnet.hr/esperanto-kel/slavonia_arbaro.pdf">
          Slavonia Arbaro</url></vrk>,
          <lok>Kroata Esperanto-Ligo, 2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_p;</trd>
  <trd lng="bg">&c_sh;&c_a;&c_r;&c_a;&c_n;</trd>
  <trd lng="br">karpenn</trd>
  <trd lng="ca">carpa</trd>
  <trd lng="cs">z&aacute;p&ecaron;st&iacute;</trd>
  <trd lng="de">Karpfen</trd>
  <trd lng="en">carp</trd>
  <trd lng="fa">&f_k;&f_p;&a_w;&a_r;</trd>
  <trd lng="fr">carpe<klr tip="ind"> (poisson)</klr></trd>
  <trd lng="hu">t&odblac;ponty</trd>
  <trd lng="nl">karper</trd>
  <trd lng="pl">karp</trd>
  <trd lng="ro">crap</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_r;&c_p;</trd>,
    <trd>&c_s;&c_a;&c_z;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="sk">z&aacute;p&auml;stn&eacute; kosti</trd>
  <trd lng="sv">karp</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&#x440;&#x43E;&#x43F;</trd>,
    <trd>&#x441;&#x430;&#x437;&#x430;&#x43D;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9BC9; <pr>l&#x1D0;</pr></trd>,
    <trd>&#x9BC9;&#x9B5A; <pr>l&#x1D0;y&#xFA;</pr></trd>
  </trdgrp>
</drv>
</art>
