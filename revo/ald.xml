<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ald.xml,v 1.31 2023/10/12 18:05:09 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>ald</rad>/o
</kap>
<drv mrk="ald.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      La plej malalta virina vo&ccirc;<ref tip="super" cel="regist1.0o.vocxo">registro</ref>:
      <ekz>
        la vo&ccirc;kategorio <tld/>o signifas en la muziko la pli malaltan
        vo&ccirc;on de amba&ubreve; inaj a&ubreve; knabovo&ccirc;oj (knab<tld/>o)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=253595">Aldo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref tip="vid" cel="sopran.0o.registro">soprano</ref>,
        mezosoprano, <tld/>o, 
        <ref tip="vid" cel="tenor.0o.vocxo">tenoro</ref>, 
        <ref tip="vid" cel="barito.0o.registro">baritono</ref>,
        <ref tip="vid" cel="bas.0o.registro">baso</ref> 
        estas la malsamaj registroj 
        de kantistoj kaj kantistinoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=30970">Vo&ccirc;o</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">contralt</trd>
    <trdgrp lng="de">
     <trd>Altstimme</trd>,
     <trd>Alt</trd>
    </trdgrp>
    <trd lng="en">alto</trd>
    <trd lng="hu">alt</trd>
    <trd lng="it">contralto</trd>
    <trd lng="nl">alt</trd>
    <trd lng="pl">alt</trd>
    <trd lng="pt">contralto</trd>
    <trd lng="sk">alt</trd>
  </snc>
  <snc>
    <dif>
      <ref tip="dif" cel="violon.ald0o">Aldviolono</ref>:
      <ekz>
        violon&ccirc;elo estas granda violono, malsupra je unu okto al
        <tld/>o.
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=254736">Violonisto</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        sur violono a&ubreve; <tld/>violono (<tld/>o) kutime trovi&gcirc;as
        menton-apogilo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=30966">Aldviolono</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="lst" cel="instru2.kord0o.MUZ" lst="voko:kordinstrumentoj">kordinstrumento</ref>
    <trd lng="ca">viola</trd>
    <trd lng="en">viola</trd>
    <trd lng="nl">altviool</trd>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_t;</trd>
  <trd lng="cs">viola</trd>
  <trd lng="fr">alto</trd>
  <trd lng="he">&kaf;&lamed;&yod; &nun;&gimel;&yod;&nun;&he;</trd>
  <trd lng="hu">m&eacute;lyheged&udblac;</trd>
  <trd lng="it">viola</trd>
  <trd lng="ja">&#x30A2;&#x30EB;&#x30C8;  <pr>&#x3042;&#x308B;&#x3068;</pr></trd>
  <trd lng="pl">alt&oacute;wka</trd>
  <trd lng="pt">violeta</trd>
  <trd lng="ru">&c_a;&c_l;&c_mol;&c_t;</trd>
  <trd lng="sk">viola</trd>
  <trd lng="uk">&c_a;&c_l;&c_mol;&c_t; (&c_g;&c_o;&c_l;&c_o;&c_s;)</trd>
</drv>
</art>
