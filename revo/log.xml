<drv mrk="log.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="log.0i.KOMUNE">
    <dif>
      Peni altiri, tentante per pla&ccirc;aj flataj rimedoj:
      <ekz>
        <tld/>i muson per lardo, knabon per kuko, knabinon per dol&ccirc;aj
        paroloj;
      </ekz><ekz>
        la knaboj <klr>[&#8230;]</klr> alfiksis fi&scirc;etojn por <tld/>i la kankrojn
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        tiun &ccirc;i instrumenton la avo tenis same kiel li <klr>[&#8230;]</klr>, el &gcirc;i li <tld/>is
        melodiojn pli belajn ol iu alia
        <fnt><bib>HsT</bib> <lok>La nepo</lok></fnt>;
      </ekz><ekz>
        se pekuloj vin <tld/>os, ne sekvu ilin
        <fnt><bib>MT</bib> <lok>&Sen; 1:10</lok></fnt>;
      </ekz><ekz>
        la promeso de tiel franda titolo <tld/>is nun multajn
        <fnt>
          <bib>Fab2</bib>
          <lok>Sonorilo</lok>
        </fnt>;
      </ekz><ekz>
        <nom>Hiram</nom> <tld/>as sur siajn &scirc;ipojn junajn knabinojn
        <klr>[&#8230;]</klr> kaj, stre&ccirc;inte la velojn, forrabas, por ilin vendi
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz><ekz>
        sekvu lin kaj <tld/>u al la &scirc;ipo
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz><ekz>
        starigi unu <klr>[&#8230;]</klr> el tiuj situacioj, <klr>[&#8230;]</klr> kiuj &ccirc;iam <tld/>as
        la rigardantojn
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz><ekz>
        e&ccirc; per dol&ccirc;a kuko vi min ne al<tld/>os
        <fnt><bib>PrV</bib></fnt>;
      </ekz><ekz>
        pli al<tld/>as kulero da mielo ol da vinagro barelo
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="es">atraer</trd>
    <trdgrp lng="hu">
      <trd>cs&aacute;b&iacute;t</trd>,
      <trd>csalogat</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_i;&c_s;&c_k;&c_u;&c_sh;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_o;&c_b;&c_l;&c_a;&c_z;&c_n;&c_ja;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="log.0i.FIGURE">
    <dif>
      <ref tip="super" cel="tir.al0i.logi">Altiri</ref>; naski intereson, deziron, pla&ccirc;on:
      <ekz>
        la teatro, danco tute ne <tld/>as min;
      </ekz><ekz>
        la tapi&scirc;o sur la planko estis dika kaj mola kiel musko, &ccirc;io <tld/>is al dormeto
        <fnt>
          <bib>Fab3</bib>
          <lok>Lasta perlo</lok>
        </fnt>;
      </ekz><ekz>
        mi spiras, ion fre&scirc;an mi sentas, la fre&scirc;o min <tld/>as
        <fnt><aut>L. L. Zamenhof</aut><vrk>Pluvo</vrk> </fnt>;
      </ekz><ekz>
        edzi&gcirc;o ne <tld/>as min
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz><ekz>
        en la brakoj de mia Amalio, <tld/>as min plezuro pli nobla
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz><ekz>
        beleco <tld/>as, virto apogas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="instig.0i">instigi</ref>,
      <ref cel="invit.0i">inviti</ref>,
      <ref cel="magnet.0a">magneta</ref>,
      <ref cel="miel.0o">mielo</ref>,
      <ref cel="tent.0i">tenti</ref>
    </refgrp>
    <trd lng="hu">vonz</trd>
	<trd lng="es">tentar</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_i;&c_v;&c_l;&c_je;&c_k;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_m;&c_a;&c_n;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_b;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_v;&c_a;&c_b;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_k;&c_u;&c_sh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>l&aacute;kat</trd>,
    <trd>v&aacute;bit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>locken</trd>,
    <trd>anlocken</trd>,
    <trd>verlocken</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>entice</trd>,
    <trd>lure<klr> (vt)</klr></trd>,
    <trd>bait<klr> (vt)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>app&acirc;ter</trd>,
    <trd>attirer</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6C17;&#12434;&#x5F15;&#x304F;  <pr>&#x304D;&#12434;&#12402;&#x304F;</pr></trd>,
    <trd>&#x5F15;&#x304D;&#x4ED8;&#12369;&#x308B;  <pr>&#12402;&#x304D;&#12388;&#12369;&#x308B;</pr></trd>,
    <trd>&#x9B45;&#x60D1;&#12377;&#x308B;  <pr>&#x307F;&#x308F;&#x304F;&#12377;&#x308B;</pr></trd>,
    <trd>&#x8A98;&#12358;  <pr>&#12373;&#x305D;&#12358;</pr></trd>,
    <trd>&#x304A;&#12403;&#x304D;&#x5BC4;&#x305B;&#x308B;
    <pr>&#x304A;&#12403;&#x304D;&#12424;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>lokken</trd>,
    <trd>aantrekken</trd>
  </trdgrp>
  <trdgrp lng="no">
    <trd>lokke</trd>,
    <trd>tiltrekke</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>n&eogonek;ci&cacute;</trd>,
    <trd>wabi&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>atrair</trd>,
    <trd>tentar</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>ademeni</trd>,
