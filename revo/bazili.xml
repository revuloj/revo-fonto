<art mrk="$Id: bazili.xml,v 1.19 2023/10/14 08:39:48 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>bazilik</rad>/o
</kap>
<drv mrk="bazili.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ARKI</uzo>
  <uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      Ortangula apudforuma <ref tip="super" cel="konstr.0ajxo">konstrua&jcirc;o</ref> kun duonrondaj
      ekstrema&jcirc;oj,
      servanta kiel ju&gcirc;ejo, borso, kc, &ccirc;e antikvaj romanoj.
    </dif>
  </snc>
  <snc>
    <uzo tip="fak">KAT</uzo>
    <dif>
      Titolo privilegie konsentita de papo al kelkaj pre&gcirc;ejoj:
      <ekz>
        estis decidite, la&ubreve; rusa kutimo, konstrui en Moskvo
        <tld/>on en la
        nomo de la plej proksima kristana festo<fnt><aut>Sergio
        Pokrovskij</aut>: <vrk>Moskvo jubileas</vrk>,
        <bib>Monato</bib><lok>jaro 1997a, numero 9a, p. 22a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_z;&c_y;&c_l;&c_ib;&c_k;&c_a;</trd>
  <trd lng="bg">&c_b;&c_a;&c_z;&c_i;&c_l;&c_i;&c_k;&c_a;</trd>
  <trd lng="cs">bazilika</trd>
  <trd lng="de">Basilika</trd>
  <trd lng="en">basilica</trd>
  <trd lng="es">bas&iacute;lica</trd>
  <trd lng="fr">basilique</trd>
  <trd lng="he">&#x5D1;&#x5D6;&#x5D9;&#x5DC;&#x5D9;&#x5E7;&#x5D4;</trd>
  <trd lng="hu">bazilika</trd>
  <trd lng="it">basilica</trd>
  <trdgrp lng="ja">
    <trd>&#x30D0;&#x30B8;&#x30EA;&#x30AB;</trd>,
    <trd>&#x30D0;&#x30B8;&#x30EA;&#x30AB;&#x8056;&#x5802;  <pr>&#x30D0;&#x30B8;&#x30EA;&#x30AB;&#x305B;&#x3044;&#x3069;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">basiliek</trd>
  <trd lng="pl">bazylika</trd>
  <trd lng="ru">&c_b;&c_a;&c_z;&c_i;&c_l;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">bazilika</trd>
  <trd lng="uk">&#x431;&#x430;&#x437;&#x438;&#x43B;&#x456;&#x43A;&#x430;</trd>
</drv>
