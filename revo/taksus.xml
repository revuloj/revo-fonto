<art mrk="$Id: taksus.xml,v 1.27 2023/09/04 17:49:39 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>taksus</rad>/o
</kap>

<drv mrk="taksus.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio
      <ref tip="malprt" cel="taksus1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Taxus</trd>), arbo el
      <ref tip="lst" cel="konifer.0o" lst="voko:koniferoj">koniferoj</ref>
      kun &ccirc;iamverdaj pinglofolietoj kaj brilru&gcirc;aj dol&ccirc;aj beroj kun venena grajno:
      <ekz>
        tiutempe en E&ubreve;ropo, Azio kaj Afriko estis &gcirc;enerale uzataj t.&#x202F;n.
        longaj arkoj, kun longeco de 150&ndash;200&#x202F;cm, el ligno, specife el <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Judit Felszeghy</aut>
          <vrk><url ref="&Monato;/2008/009409.php">Ne 
          nur sporto, anka&ubreve; vivoformo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/e/ed/Taxus_baccata_Lviv.JPG/320px-Taxus_baccata_Lviv.JPG" prm="CC BY-SA 3.0">bran&ccirc;o kun beroj de E&ubreve;ropa <tld/>o (Taxus baccata), Lviv
        <fnt>
          <aut>Mykola Swarnyk, propra verko</aut>
          <lok><url ref="&WCW;/File:Taxus_baccata_Lviv.JPG">&WCW;/File:Taxus_baccata_Lviv.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_c;&c_ib;&c_s;</trd>
  <trd lng="bg">&c_t;&c_i;&c_s;</trd>
  <trd lng="ca">teix</trd>
  <trd lng="cs">tis</trd>
  <trd lng="de">Eibe</trd>
  <trd lng="en">yew</trd>
  <trd lng="fr">if</trd>
  <trd lng="hu">tiszafa</trd>
  <trd lng="ja">&#x30A4;&#x30C1;&#x30A4;</trd>
  <trd lng="nl">taxus</trd>
  <trd lng="pl">cis</trd>
  <trdgrp lng="pt">
   <trd>teixo</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_i;&c_s;&c_s;</trd>
  <trd lng="sk">tis</trd>
  <trd lng="sv">idegran</trd>
  <trd lng="uk">&#x442;&#x438;&#x441;</trd>
</drv>
</art>
