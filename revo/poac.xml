<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: poac.xml,v 1.18 2024/08/14 18:32:30 revo Exp $">
<kap>
  <rad>poac</rad>/oj
  <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="poac.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="poac.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref>  de <ref
      tip="malprt" cel="kotile.unu0aj">unukotiledonaj</ref>
      herboj (<trd lng="la">Poaceae</trd>), kun pli ol 300 genroj:
      <refgrp tip="sub">
        <ref cel="aven.0o">aveno</ref>,
        <ref cel="horde.0o">hordeo</ref>,
        <ref cel="maiz.0o">maizo</ref>,
        <ref cel="sekal.0o">sekalo</ref>,
        <ref cel="tritik.0o">tritiko</ref>:
      </refgrp>
      <ekz>
        glumo estas &ccirc;iu el la du brakteoj, kovrantaj la spiketon de <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19040">Glumo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  <trdgrp lng="id">
    <trd>Glumiflorae</trd>,
    <trd>Graminae</trd>,
    <trd>Poaceae</trd>,
    <trd><baz>padi</baz>padi-padian</trd>,
    <trd><baz>rumput</baz>rumput-rumputan</trd>
  </trdgrp>
  </snc>
  <bld lok="../bld/po1.png">
    folio<klr>:
    <refgrp tip="prt">
    1. foliplato (<ref cel="limb.0o.de_folio">limbo</ref>), 2. <ref cel="ligul.0o">ligulo</ref>, 3. oreleto, 4.
    foliingo, 5. folinodo</refgrp></klr>;
    floro<klr>:
    <refgrp tip="prt">
      a. <ref cel="stamen.0o">stameno</ref>, b. <ref cel="pistil.0o">pistilo</ref>, c. <ref cel="lodikl.0o">lodiklo</ref>,
      d. interna <ref cel="pale.0o">paleo</ref>, e. ekstera paleo, f. <ref cel="arist.0o.BOT">aristo</ref></refgrp></klr>;
    spiketo<klr>:
    <refgrp tip="prt">
      A. ekstera <ref cel="glum.0o">glumo</ref>, B. interna glumo, C. unuopa floro
    </refgrp></klr>
  </bld>
  <trd lng="be">&c_z;&c_l;&c_a;&c_k;&c_ib; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  <trdgrp lng="ca">
    <trd>po&agrave;cies</trd>,
    <trd>gram&iacute;nies</trd>
  </trdgrp>
  <trd lng="de">S&uuml;&szlig;gr&auml;ser</trd>
  <trdgrp lng="en">
    <trd>poaceae</trd>,
    <trd>gramineae</trd>
    <trd>true grasses</trd>
  </trdgrp>
  <trd lng="es">gram&iacute;neas</trd>
  <trdgrp lng="hu">
    <trd>p&aacute;zsitf&udblac;f&eacute;l&eacute;k</trd>,
    <trd>perjef&eacute;l&eacute;k</trd>
  </trdgrp>
  <trd lng="nl">echte <ind>grassen</ind><klr tip="ind">
  (fam.)</klr></trd>
  <trdgrp lng="pl">
    <trd>wiechlinowate</trd>,
    <trd>trawy</trd>
  </trdgrp>
  <trd lng="pt">po&aacute;ceas</trd>
  <trd lng="ru">&c_z;&c_l;&c_a;&c_k;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sv">gr&auml;s</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_l;&c_a;&c_k;&c_i;</trd>,
    <trd>&c_z;&c_l;&c_a;&c_k;&c_o;&c_v;&c_ib;</trd>
  </trdgrp>
  <trd lng="zh">&#31166;&#26412;&#31185; <pr>h&eacute;b&ecaron;nk&emacron;</pr></trd>
</drv>
</art>

