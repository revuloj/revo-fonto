<vortaro>
<art mrk="$Id$">
<kap>
<rad>fas&ccirc;in</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="fascxi.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Fasko el bran&ccirc;etoj por fari barojn, &scirc;topplenigi fosa&jcirc;ojn ktp,
      precipe uzata en militarto. 
    </dif>
    <refgrp tip="vid">
      <ref cel="gabi.0o">gabio</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_sh;&c_y;&c_n;&c_a;</trd>,
    <trd>&c_g;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">Faschine</trd>
  <trd lng="fr">fascine</trd>
  <trdgrp lng="hu">
    <trd>r&odblac;zsek&ouml;teg</trd>,
    <trd>r&odblac;zsenyal&aacute;b</trd>
  </trdgrp>
  <trd lng="nl">fascine</trd>
  <trd lng="pt">faxina <klr tip="amb">(feixe de ramos ou paus)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_a;&c_sh;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_g;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
