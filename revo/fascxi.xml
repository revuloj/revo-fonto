<kap>
  <rad>fas&ccirc;in</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="fascxi.0o">
  <kap><tld/>o</kap>
  <snc mrk="fascxi.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="super" cel="fask.0o">Fasko</ref> el bran&ccirc;etoj por fari barojn, &scirc;topplenigi
      fosa&jcirc;ojn ktp, precipe uzata en militarto:
      <ekz>
        vi ja estus povinta ordigi la brul-<tld/>ojn
        <fnt>
          <aut>V. Catal&agrave;, trad. J. Ventura i Freixas</aut>,
          <vrk>Soleco</vrk>,
          <lok>1967</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gabi.0o">gabio</ref>
    </refgrp>
    <bld
      lok="&WCU;/thumb/0/0b/LahnungSuedwesthoern.jpg/640px-LahnungSuedwesthoern.jpg"
      prm="PD"
      ><tld/>oj &ccirc;e marbordo
        <fnt>
          <aut>Agnete, propra verko</aut>
          <lok><url ref="&WCW;/File:LahnungSuedwesthoern.jpg"
              >https://commons.wikimedia.org/wiki/File:LahnungSuedwesthoern.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_sh;&c_y;&c_n;&c_a;</trd>,
    <trd>&c_g;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Faschine</trd>,
    <trd>Stackbusch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>fascine</trd>,
    <trd>faggot (of brushwood)</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fagot</trd>,
    <trd>fascine</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>r&odblac;zsek&ouml;teg</trd>,
    <trd>r&odblac;zsenyal&aacute;b</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#31895;&#26422;  <pr>&#12381;&#12384;</pr></trd>,
    <trd>&#26463;&#26612;  <pr>&#12383;&#12400;&#12375;&#12400;</pr></trd>
  </trdgrp>
  <trd lng="nl">fascine</trd>
  <trd lng="pl">faszyna</trd>
  <trd lng="pt">faxina <klr tip="amb">(feixe de ramos ou
  paus)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_a;&c_sh;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_g;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
