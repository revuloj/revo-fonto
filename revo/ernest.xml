<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ernest.xml,v 1.15 2023/08/31 11:49:30 revo Exp $">
<kap>
  <ofc>*</ofc> <ofc>7</ofc> <rad>Ernest</rad>/o
</kap>

<drv mrk="ernest.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">NOM</uzo>
  <snc>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        el &ccirc;iuj miaj infanoj <tld/>o estas la plej juna
        <fnt><bib>F</bib>, <lok>&FE; 10</lok></fnt>.
      </ekz>
    </dif>
  </snc>           
  <trd lng="br">Ernest</trd>
  <trd lng="ca">Ernest</trd>
  <trd lng="de">Ernst</trd>
  <trd lng="da">Ernst</trd>
  <trd lng="en">Ernest</trd>
  <trd lng="es">Ernesto</trd>
  <trd lng="fr">Ernest</trd>
  <trd lng="it">Ernesto</trd>
  <trdgrp lng="ja">
    <trd>&#x30A8;&#x30EB;&#x30CD;&#x30B9;&#x30C8;</trd>,
    <trd>&#x30A2;&#x30FC;&#x30CD;&#x30B9;&#x30C8;</trd>
  </trdgrp>
  <trd lng="no">Ernst</trd>
  <trd lng="pl">Ernest</trd>
  <trd lng="pt">Ernesto</trd>
  <trdgrp lng="ru">
    <trd>&c_E;&c_r;&c_n;&c_je;&c_s;&c_t;</trd>,
    <trd>&c_E;&c_r;&c_n;&c_s;&c_t;</trd>
  </trdgrp>
  <trd lng="sl">Ernest</trd>
  <trd lng="sv">Ernst</trd>
  <trdgrp lng="uk">
    <trd>&#x415;&#x440;&#x43D;&#x441;&#x442;</trd>,
    <trd>&#x415;&#x440;&#x43D;&#x435;&#x441;&#x442;</trd>
  </trdgrp>
</drv>

<drv mrk="ernest.0cxjo">
  <kap><ofc>*</ofc> Erne&ccirc;jo, <var><kap>Er&ccirc;jo</kap></var></kap>
  <snc>
   <dif>
    Diminutivo de Ernesto:
      <ekz>
        Johanon, ... Erneston, ... iliaj gepatroj nomas Johan&ccirc;jo (a&ubreve;
        Jo&ccirc;jo), ... Erne&ccirc;jo (a&ubreve; Er&ccirc;jo), ...
        <fnt><bib>F</bib>, <lok>&FE; 38</lok></fnt>.
      </ekz> 
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>N&eacute;nesse</trd>
    <trd>N&egrave;nesse</trd>
  </trdgrp>
  <trd lng="pt">Nesto</trd>
</drv>

<drv mrk="ernest.0ino">
  <kap>Ernestino, <var><kap>Ernenjo</kap></var>,
  <var><kap>Ernjo</kap></var></kap>
  <snc>
   <dif>
      Virina <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena
    </dif>
  </snc>
  <trd lng="en">Ernestine</trd>
  <trd lng="es">Ernestina</trd>
  <trd lng="fr">Ernestine</trd>
  <trd lng="pt">Ernestina</trd>
  <trd lng="sl">Ernestina</trd>
</drv>

</art>

