<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>fe</rad>/o
</kap>
<drv mrk="fe.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">MIT</uzo>
   <snc mrk="fe.0o.MIT">
     <dif>
       Fabela homforma esta&jcirc;o kun magia povo.
     </dif>
   </snc>
   <trdgrp lng="de">
     <trd>Genius</trd>,
     <trd>guter <ind>Geist</ind></trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>fairy</trd>,
     <trd>fay</trd>,
     <trd>spirit</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>&ecirc;tre surnaturel</trd>,
     <trd>g&eacute;nie<klr tip="ind"> (&ecirc;tre surnaturel)</klr>
     </trd>
   </trdgrp>
   <trd lng="hu">t&uuml;nd&eacute;r</trd>
   <trd lng="es">genio</trd>
   <trdgrp lng="pl">
     <trd>wr&oacute;&zdot;<klr> (pan wr&oacute;&zdot;ka)</klr></trd>,
     <trd>duch</trd>,
     <trd>chochlik</trd>
   </trdgrp>
   <trd lng="pt">g&ecirc;nio</trd>
   <trdgrp lng="ru">
     <trd>&c_d;&c_u;&c_h;</trd>,
     <trd>
     &c_s;&c_k;&c_a;&c_z;&c_o;&c_ch;&c_n;&c_o;&c_je;
     &c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;
     </trd>
   </trdgrp>
</drv>

<drv mrk="fe.0a">
   <kap><tld/>a</kap>
   <snc mrk="fe.0a.FIG">
     <uzo tip="stl">FIG</uzo>
     <dif>
       Mirinde bela, kvaza&ubreve; elsor&ccirc;ita de <tld/>o:
       <ekz>
         <ind><tld/>a</ind> iluminado.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="en">
     <trd>fairy</trd>,
     <trd>faerie</trd>,
     <trd>faery</trd>,
     <trd>spirit</trd>
   </trdgrp>
   <trd lng="fr">f&eacute;erique</trd>
   <trd lng="es">m&aacute;gico, -a</trd>
   <trd lng="hu">t&uuml;nd&eacute;ri</trd>
   <trd lng="nl">fee&euml;riek</trd>
   <trd lng="pl">bajeczny</trd>
</drv>

<drv mrk="fe.0ino">
   <kap><ofc>*</ofc><tld/>ino</kap>
   <snc mrk="fe.0ino.MIT">
     <dif>
       Ina feo:
       <ekz>
         tio &ccirc;i estis <tld/>ino, kiu prenis sur sin la formon de
         malri&ccirc;a vila&gcirc;a virino
         <fnt><bib>F</bib>, <lok>&FE; 15</lok></fnt>;
       </ekz>
       <ekz>
         vartistinaj rakontoj pri <tld/>inoj.
       </ekz>
     </dif>
   </snc>
   <trd lng="de">Fee</trd>
   <trd lng="fr">f&eacute;e</trd>
   <trd lng="es">hada</trd>
   <trd lng="nl">fee</trd>
   <trd lng="hu">t&uuml;nd&eacute;rl&aacute;ny</trd>
   <trdgrp lng="pl">
     <trd>wr&oacute;&zdot;ka</trd>,
     <trd>wieszczka<klr> (przest.)</klr></trd>
   </trdgrp>
   <trd lng="pt">fada</trd>
   <trd lng="ru">&c_f;&c_je;&c_ja;</trd>
</drv>
</art>
