<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fe</rad>/o
</kap>
<drv mrk="fe.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Fabela esta&jcirc;o kun supernatura povo:
      <ekz>
        vartistinaj rakontoj pri <ind><tld/>ino</ind>j
        <trd lng="de">Fee</trd>
        <trd lng="ru">&c_f;&c_je;&c_ja;</trd>
        <klr>(f)</klr>;
      </ekz>
      <ekz>
        <tld/>a (mirinde bela) iluminado.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kobold.0o">koboldo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Genius</trd>,
     <trd>guter <ind>Geist</ind></trd></trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_u;&c_h;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_z;&c_o;&c_ch;&c_n;&c_o;&c_je; &c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
</drv>
</art>

