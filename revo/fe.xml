<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fe.xml,v 1.28 2023/11/09 19:13:17 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>fe</rad>/o
</kap>
<drv mrk="fe.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">MIT</uzo>
   <snc mrk="fe.0o.MIT">
     <dif>
      Fabela homforma <ref tip="super" cel="est.0ajxo.ulo">esta&jcirc;o</ref> kun <ref cel="magi.0a.MAG">magia</ref> povo:
      <ekz>
        etaj <tld/>oj kun marteloj
        <fnt>
          <aut>Grigori Arosev</aut>
            <vrk><url ref="&Monato;/2000/007198.php">Poezio sed ne poemoj</url></vrk>
            <bib>Monato</bib>
            <lok>2000/12, p. 28</lok>
          </fnt>;
      </ekz>
      <ekz>
        se iu bona <tld/>o vizitus la eldonejon, kaj se la oficistoj rajtus esprimi tri
        dezirojn, ne mankus ideoj
        <fnt>
          <bib>Monato</bib>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="&Monato;/2004/008280.html">Komposta revolucio kaj presada
          stabileco</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        trikapaj troloj, barbaj nanoj, lumineskantaj <tld/>oj, kaprokruraj fa&ubreve;noj
        <klr>[&#x2026;]</klr> kaj sennombraj aliaj viva&jcirc;oj movis sin la&ubreve; la strato
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Fantaziujo en Dan&gcirc;ero</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
       <ref cel="elf.0o">elfo</ref>,
       <ref cel="silf.0o">silfo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_u;&c_h; <klr>(&c_k;&c_a;&c_z;&c_a;&c_ch;&c_n;&c_y;)</klr></trd>
  <trdgrp lng="bg">
     <trd>&c_f;&c_je;&c_ja;</trd>,
     <trd>&c_s;&c_a;&c_m;&c_o;&c_d;&c_i;&c_v;&c_a;</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>poh&aacute;dkov&aacute; bytost</trd>,
    <trd>sk&rcaron;&iacute;tek</trd>,
    <trd>&scaron;otek</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Genius</trd>,
     <trd>guter <ind>Geist</ind></trd>
   </trdgrp>
  <trdgrp lng="en">
     <trd>fairy</trd>,
     <trd>fay</trd>,
     <trd>spirit</trd>
   </trdgrp>
  <trd lng="es">genio</trd>
  <trdgrp lng="fr">
     <trd>&ecirc;tre surnaturel</trd>,
     <trd>g&eacute;nie<klr tip="ind"> (&ecirc;tre surnaturel)</klr>
     </trd>
   </trdgrp>
  <trd lng="hu">t&uuml;nd&eacute;r</trd>
  <trd lng="ja">&#x5996;&#x7CBE;  <pr>&#x3088;&#x3046;&#x305B;&#x3044;</pr></trd>
  <trdgrp lng="pl">
     <trd>wr&oacute;&zdot;<klr> (pan wr&oacute;&zdot;ka)</klr></trd>,
     <trd>duch</trd>,
     <trd>chochlik</trd>
   </trdgrp>
  <trd lng="pt">g&ecirc;nio</trd>
  <trdgrp lng="ru">
     <trd>&c_d;&c_u;&c_h;</trd>,
     <trd>
     &c_s;&c_k;&c_a;&c_z;&c_o;&c_ch;&c_n;&c_o;&c_je;
     &c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;
     </trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>rozpr&aacute;vkov&aacute; bytos&tcaron;</trd>,
    <trd>&scaron;kriatok</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_u;&c_h;</trd>,
    <trd>&#x43C;&c_ib;&c_f;&c_ib;&c_ch;&#x43D;&c_a; &c_ib;&c_s;&c_t;&#x43E;&c_t;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="fe.0a">
   <kap><tld/>a</kap>
   <snc mrk="fe.0a.FIG">
     <uzo tip="stl">FIG</uzo>
     <dif>
       Mirinde <ref tip="super" cel="bel.0a">bela</ref>, kvaza&ubreve; elsor&ccirc;ita de <tld/>o:
       <ekz>
         <ind><tld/>a</ind> iluminado;
       </ekz>
      <ekz>
        en <tld/>a blanko ankora&ubreve; brilis, senmakula kaj netu&scirc;ita, la Ebura Turo
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Flugo al la Ebura Turo</lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_ch;&c_a;&c_r;&c_o;&c_w;&c_n;&c_y;</trd>
  <trdgrp lng="bg">
     <trd>&c_p;&c_r;&c_i;&c_k;&c_a;&c_z;&c_je;&c_n;</trd>,
     <trd>&c_v;&c_malmol;&c_l;&c_sh;&c_je;&c_b;&c_je;&c_n;</trd>,
     <trd>&c_s;&c_a;&c_m;&c_o;&c_d;&c_i;&c_v;&c_s;&c_k;&c_i;</trd>
   </trdgrp>
  <trd lng="cs">poh&aacute;dkov&yacute;</trd>
  <trd lng="de">feenhaft</trd>
  <trdgrp lng="en">
     <trd>fairy</trd>,
     <trd>faerie</trd>,
     <trd>faery</trd>,
     <trd>spirit</trd>
   </trdgrp>
  <trdgrp lng="es">
    <trd>m&aacute;gico, -a</trd>
    <trd>maravilloso</trd>
  </trdgrp>
  <trd lng="fr">f&eacute;erique</trd>
  <trd lng="hu">t&uuml;nd&eacute;ri</trd>
  <trdgrp lng="ja">
    <trd>&#x5996;&#x7CBE;&#x306E;  <pr>&#x3088;&#x3046;&#x305B;&#x3044;&#x306E;</pr></trd>,
    <trd>&#x5922;&#x306E;&#x3088;&#x3046;&#x306B;&#x7F8E;&#x3057;&#x3044;  <pr>&#x3086;&#x3081;&#x306E;&#x3088;&#x3046;&#x306B;&#x3046;&#x3064;&#x304F;&#x3057;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">fee&euml;riek</trd>
  <trd lng="pl">bajeczny</trd>
  <trd lng="sk">rozpr&aacute;vkov&yacute;</trd>
  <trdgrp lng="zh">
    <trd>&#x50CF;&#x4ED9;&#x5973; <pr>xi&#xE0;ngxi&#x101;nn&#x1DA;</pr></trd>,
    <trd>&#x4ED9;&#x5973; <pr>xi&#x101;nn&#x1DA;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fe.0ino">
   <kap><ofc>*</ofc><tld/>ino</kap>
   <snc mrk="fe.0ino.MIT">
     <dif>
       Ina feo:
       <ekz>
         tio &ccirc;i estis <tld/>ino, kiu prenis sur sin la formon de
         malri&ccirc;a vila&gcirc;a virino
         <fnt><bib>F</bib>, <lok>&FE; 15</lok></fnt>;
       </ekz>
      <ekz>
        la <tld/>ino de Mal&gcirc;ojo
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la belega, konstante &scirc;an&gcirc;i&gcirc;anta nuba palaco de la <tld/>ino <nom>Fata-Morgana</nom>
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        aminde &ccirc;arma, kiel povas esti nur la <tld/>ino en la &gcirc;ardeno de la paradizo
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_e;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>f&eacute;ja</trd>,
    <trd>v&iacute;la</trd>
  </trdgrp>
  <trd lng="de">Fee</trd>
  <trd lng="es">hada</trd>
  <trd lng="fr">f&eacute;e</trd>
  <trd lng="hu">t&uuml;nd&eacute;rl&aacute;ny</trd>
  <trd lng="ja">&#x5996;&#x7CBE;  <pr>&#x3088;&#x3046;&#x305B;&#x3044;</pr></trd>
  <trd lng="nl">fee</trd>
  <trdgrp lng="pl">
     <trd>wr&oacute;&zdot;ka</trd>,
     <trd>wieszczka<klr> (przest.)</klr></trd>
   </trdgrp>
  <trd lng="pt">fada</trd>
  <trd lng="ru">&c_f;&c_je;&c_ja;</trd>
  <trd lng="sk">v&iacute;la</trd>
  <trdgrp lng="uk">
    <trd>&c_f;&c_je;&#x44F;</trd>,
    <trd>&#x43C;&c_ib;&c_f;&c_ib;&c_ch;&#x43D;&c_a; &c_ib;&c_s;&c_t;&#x43E;&c_t;&c_a; &c_zh;&c_ib;&#x43D;&#x43E;&c_ch;&#x43E;&c_ji; &c_s;&c_t;&c_a;&c_t;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7F8E;&#x4E3D;&#x5973;&#x5996; <pr>m&#x11B;il&#xEC;n&#x1DA;y&#x101;o</pr></trd>,
    <trd>&#x5929;&#x5973; <pr>ti&#x101;nn&#x1DA;</pr></trd>,
    <trd>&#x5929;&#x4ED9; <pr>ti&#x101;nxi&#x101;n</pr></trd>,
    <trd>&#x4ED9;&#x5973; <pr>xi&#x101;nn&#x1DA;</pr></trd>
  </trdgrp>
</drv>
</art>
