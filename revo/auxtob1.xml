<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>a&ubreve;tobus</rad>/o
</kap>

<drv mrk="auxtob1.0o">
  <kap><tld/>o</kap>
  <snc mrk="auxtob1.0o.AUT">
  <uzo tip="fak">AUT</uzo>
    <dif>
      Granda <ref tip="super" cel="auxtom1.0o">a&ubreve;tomobilo</ref> por
      publika transportado de pluraj personoj samtempe, &ccirc;efe
      en urbo a&ubreve; en &gcirc;ia &ccirc;irka&ubreve;a&jcirc;o:
      <ekz>
        turismaj <tld/>oj revenis duonplenaj al Litovio<fnt><aut>Antanas
        Grin&ccaron;evi&ccaron;ius</aut>: <vrk>Ruze eniri, akuze eliri</vrk>,
        <bib>Monato</bib><lok>jaro 2000a, numero 11a, p. 6a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="bus.0o">buso</ref>
    </refgrp>
    <rim mrk="auxtob1.0o.ETIM">
      Historie temas pri <ctl>a&ubreve;tomobila omnibuso</ctl>.
    </rim>
  </snc>
  <trd lng="be">&c_a;&c_w;&c_t;&c_o;&c_b;&c_u;&c_s;</trd>
  <trd lng="bg">&c_a;&c_v;&c_t;&c_o;&c_b;&c_u;&c_s;</trd>
  <trdgrp lng="de">
    <trd>Omnibus</trd>,
    <trd>Bus</trd>
    <trd lng="el">&lambda;&epsilon;&omega;&phi;&omikron;&rho;&epsilon;&jota_ton;&omikron;</trd>
  </trdgrp>
  <trd lng="en">bus</trd>
  <trd lng="fa">&a_A;&a_t;&a_w;&a_b;&a_w;&a_s;</trd>
  <trd lng="fr">autobus</trd>
  <trdgrp lng="es">
    <trd>autob&uacute;s</trd>,
    <trd>cami&oacute;n</trd>
  </trdgrp>
  <trd lng="he">&alef;&vav;&tet;&vav;&bet;&vav;&samekh;</trd>
  <trdgrp lng="hu">
    <trd>busz</trd>,
    <trd>aut&oacute;busz</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>autobus</trd>,
    <trd>bus</trd>
  </trdgrp>
  <trd lng="nl">autobus</trd>
  <trd lng="pl">autobus</trd>
  <trd lng="pt">&ocirc;nibus</trd>
  <trd lng="ru">&c_a;&c_v;&c_t;&c_o;&c_b;&c_u;&c_s;</trd>
  <trd lng="sk">autobus</trd>
  <trd lng="sv">bus</trd>
</drv>
</art>
