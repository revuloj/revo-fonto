<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vojevod.xml,v 1.9 2021/04/12 07:08:00 revo Exp $">
<kap>
  <rad>vojevod</rad>/o <fnt><bib>FK</bib></fnt>
</kap>
<drv mrk="vojevod.0o">
  <kap><tld/>o</kap>
  <snc mrk="vojevod.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <klr>(en Pollando ekde la 14a jc)</klr>
      <ref tip="super" cel="gubern1.0estro.POL">Guberniestro</ref>,
      estro de <ref tip="dif" cel="vojevodi.0o.POL">vojevodio</ref>:
      <ekz>
        en vespero somera <tld/>o kolera /
        al la hejma kastelo rapidas
        <fnt>
        <aut>A. Mickiewicz, trad. L. L. Zamenhof</aut>
        <vrk>La Vojevodo</vrk>
        <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj ne oscedis, /
        tamen vane ja procedis
        <fnt>
        <aut>A. S. Pu&scirc;kin, trad. N. Fedotov</aut>,
        <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Poezio/ora_koketou.html">
        Fabelo pri la ora koketo</url></vrk>,
        <lok>Paco, 1989:1, p. 6a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="duk.0o">duko</ref>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_ja;&c_v;&c_o;&c_d;&c_a;</trd>
  <trd lng="ca">voivoda</trd>
  <trdgrp lng="cs">
    <trd>vojvoda</trd>,
    <trd>v&eacute;voda</trd>
  </trdgrp>
  <trd lng="de">Woiwode</trd>
  <trd lng="en">voivode</trd>
  <trd lng="fr">vo&iuml;vode</trd>
  <trd lng="hu">vajda</trd>
  <trd lng="pl">wojewoda</trd>
  <trdgrp lng="sk">
    <trd>vojvoda</trd>,
    <trd>vojvodca</trd>
  </trdgrp>
</drv>

<drv mrk="vojevod.0ujo">
  <kap><tld/>ujo, <var><kap><tld/>lando</kap></var></kap>
  <snc mrk="vojevod.0ujo.POL">
    <dif>
      <ref tip="dif" cel="vojevodi.0o">Vojevodio</ref>:
      <ekz>
        li gravi&gcirc;is en <tld/>lando, /
        havante Soplicanojn kvaza&ubreve; sub komando
        <fnt>
          <aut>A. Mickiewicz, trad. A. Grabowski</aut>
          <vrk>Sinjoro Tadeo,
          <url ref="http://esperanto.pl/page.php?tid=3005">
          libro dua</url></vrk>
          <lok>1918</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="duk.0ujo">dukujo</ref>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_ja;&c_v;&c_o;&c_d;&c_s;&c_t;&c_v;&c_a;</trd>
  <trd lng="ca">voivodat</trd>
  <trd lng="de">Woiwodschaft</trd>
  <trd lng="fr">vo&iuml;vodie</trd>
  <trd lng="hu">vajdas&aacute;g</trd>
  <trd lng="pl">wojew&oacute;dztwo</trd>
</drv>
</art>

