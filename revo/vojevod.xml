<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>vojevod</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="vojevod.0o">
  <kap><tld/>o</kap>
  <snc mrk="vojevod.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="gubern1.0estro.POL">Guberniestro</ref>,
      estro de <ref tip="dif" cel="vojevodi.0o.POL">vojevodio</ref>:
      <ekz>
        En vespero somera <tld/>o kolera /
        Al la hejma kastelo rapidas
        <fnt>
        <aut>A. Mickiewicz, trad. L. L. Zamenhof</aut>
        <vrk>La Vojevodo</vrk>
        <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld lit="V"/>oj ne oscedis, /
        tamen vane ja procedis
        <fnt>
        <aut>A. S. Pu&scirc;kin, trad. N. Fedotov</aut>,
        <vrk><url
        ref="http://donh.best.vwh.net/Esperanto/Literaturo/Poezio/ora_koketou.html">
        Fabelo pri la ora koketo</url></vrk>,
        <lok>Paco, 1989:1, p. 6a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">voivode</trd>
  <trd lng="fr">vo&iuml;vode</trd>
  <trd lng="hu">vajda</trd>
</drv>

<drv mrk="vojevod.0ujo">
  <kap><tld/>ujo, <var><kap><tld/>lando</kap></var></kap>
  <snc mrk="vojevod.0ujo.POL">
    <ref tip="dif" cel="vojevodi.0o">vojevodio</ref>
    <ekz>
      <klr>...</klr> li gravi&gcirc;is en <tld/>lando, /
      Havante Soplicanojn kvaza&ubreve; sub komando
      <fnt>
      <aut>A. Mickiewicz, trad. A. Grabowski</aut>
      <vrk>Sinjoro Tadeo,
      <url ref="http://esperanto.pl/page.php?tid=3005">
      libro dua</url></vrk>
      <lok>1918</lok>
      </fnt>;
    </ekz>
  </snc>
  <trd lng="fr">vo&iuml;vodie</trd>
  <trd lng="hu">vajdas&aacute;g</trd>
</drv>
</art>

