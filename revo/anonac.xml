<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: anonac.xml,v 1.3 2023/10/16 12:43:58 revo Exp $">
  <kap>
    <rad>anonac</rad>/oj
  </kap>
<drv mrk="anonac.0oj">
  <kap>
    <tld/>oj
  </kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref>  de
      lignaj <ref tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref> plantoj
      (<trd lng="la">Annonaceae</trd>) kun plurfruktoj:
      <ekz>
        la anonacoj a&ubreve; sukerpomofamilio estas familio de angiospermoj konsistantaj el
        arboj, arbedoj a&ubreve; malofte lianoj kun proksimume 2&#8239;300 &gcirc;is 2&#8239;500 specioj kaj
        pli ol 130 genroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=372918"
              >Anonacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="anon.0arbo">anonarbo</ref>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_o;&c_n;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  <trdgrp lng="de">
    <trd>Annonengew&auml;chse</trd>,
    <trd>Flaschenbaumgew&auml;chse</trd>,
    <trd>Rahmapfelgew&auml;chse</trd>,
    <trd>Schuppenapfelgew&auml;chse</trd>
  </trdgrp>
</drv>
</art>

