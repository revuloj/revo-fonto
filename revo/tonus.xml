<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tonus.xml,v 1.9 2024/05/06 19:20:04 revo Exp $">
<kap>
  <rad>tonus</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tonus.0o">
  <kap><tld/>o</kap>
  <snc mrk="tonus.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Normala ioma <ref tip="super" cel="strecx.0o.stato">stre&ccirc;o</ref> en muskoloj, angioj...:
      <ekz>
        homaj sanstatoj esprimi&gcirc;as per momenta muskola <tld/>o
        <fnt>
          <aut>Internacia Naturkuraca Asocio</aut>
          <vrk><url ref="http://www.esp-ina.com/revu_april2005.htm">Revuoj</url></vrk>,
          <lok>2005-04</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Tonus</trd>
  </snc>
  <snc mrk="tonus.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      <ref tip="dif" cel="vigl.0o.KOMUNE">Viglo</ref>:
      <ekz>
        altranguloj malaprobas melankoliajn kantojn de junularo,
        senbaze opiniante, ke ili malaltigas jam sen tio malaltan
        <tld/>on de vivo
        <fnt><aut>Ivan Efremov, trad. Jurij Finkel</aut>, <vrk><url
        ref="http://www.esperanto.mv.ru/Efremov">Horo de
        Bovo</url></vrk>,
        <lok>&ccirc;apitro 10a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_o;&c_n;&c_u;&c_s;</trd>
  <trdgrp lng="ca">
    <trd>to</trd>,
    <trd>tonicitat</trd>
  </trdgrp>
  <trd lng="cs">tonus</trd>
  <trdgrp lng="fr">
    <trd>tonicit&eacute;</trd>,
    <trd>tonus</trd>
  </trdgrp>
  <trd lng="pl">tonus</trd>
  <trd lng="sk">tonus</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&#x43E;&#x43D;&c_u;&c_s;</trd>,
    <trd>&#x43D;&c_a;&#x43F;&c_r;&c_u;&c_g;&c_a;</trd>,
    <trd>&c_t;&c_i;&c_s;&#x43A;</trd>
  </trdgrp>
</drv>
</art>

