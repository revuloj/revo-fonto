<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>apretur</rad>/o
</kap>
<drv mrk="apretu.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Speciala glazuro por finpretigi teksa&jcirc;ojn, ledojn kc.
    </dif>
  </snc>
  <trd lng="de">Appretur</trd>
  <trd lng="en">appreture</trd>
  <trd lng="fr">appr&ecirc;t</trd>
  <trdgrp lng="hu">
    <trd>kik&eacute;sz&iacute;t&eacute;s<klr tip="amb"> (textil&eacute;)</klr></trd>,
    <trd>csinoz&aacute;s</trd>,
    <trd>appret&uacute;ra</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_p;&c_p;&c_r;&c_je;&c_t;&c_u;&c_r;&c_a;</trd>,
    <trd>&c_l;&c_o;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">apret&aacute;cia</trd>
</drv>

<drv mrk="apretu.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Finpretigi per tia glazuro.
    </dif>
  </snc>
  <trd lng="de">Appretur aufbringen</trd>
  <trd lng="fr">appr&ecirc;ter</trd>
  <trdgrp lng="hu">
    <trd>kik&eacute;sz&iacute;t<klr tip="amb"> (textilt)</klr></trd>,
    <trd>csinoz</trd>,
    <trd>appret&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_p;&c_p;&c_r;&c_je;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_l;&c_o;&c_shch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">apretova&tcaron;</trd>
</drv>
</art>
