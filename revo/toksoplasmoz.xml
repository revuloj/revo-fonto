<vortaro>
  <art mrk="$Id: toksoplasmoz.xml,v 1.2 2023/05/28 15:25:22 afido Exp $">
    <kap>
      <rad>toksoplasmoz</rad>/o
    </kap>

<drv mrk="toksoplasmoz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Parazita <ref tip="lst" lst="voko:malsanoj" cel="san.mal0o.MED">malsano</ref> precipe de
      katoj:
      <ekz>
        alia aidoso-rilata protozoa malsano, <tld/>o, estas ka&ubreve;zita de protozooj
        <fnt>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2007/008991.php"
              >La &ccirc;efaj murdistoj de HIV-infektitoj</url></vrk>
              <bib>Monato</bib>
          <lok>2007/01, p. 19</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o povas okazigi malfortajn kaj sensimptomajn infektojn, same kiel mortigajn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=697140"
              >Toksoplasmozo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Toxoplasmose</trd>
    <trd lng="en">toxoplasmosis</trd>
    <trd lng="fr">toxoplasmose</trd>
    <trd lng="nl">toxoplasmose</trd>
  </snc>
</drv>

  </art>

