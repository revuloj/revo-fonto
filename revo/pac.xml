<drv mrk="pac.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pac.0igi.akordigi">
    <dif>
      Fari, ke iu a&ubreve; io restu <tld/>a, <ref cel="akord.0igi">akordigi</ref>: 
      <ekz>
        Li e&ccirc; liajn malamikojn <tld/>igas kun li 
        <fnt><bib>MT</bib>, <lok>&Sen; 16:7</lok></fnt>;
      </ekz>
      <ekz>
        jen estas la sola rimedo por <tld/>igi E&ubreve;ropon;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>igi la logikon kun la tradicio
        <fnt>
          <bib>LR</bib>
          <lok>2. Transskribo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="he">&lamed;&he;&shin;&lamed;&yod;&fmem; 
\<ind>&shin;&lamed;&fmem;</ind>\</trd>
  </snc>
  <snc mrk="pac.0igi.denove">
     <dif>
       Fari ke iu a&ubreve; io estu denove <tld/>a:
       <ekz>
         <tld/>igi malkonsenton, ribelon, disputon;
       </ekz>
       <ekz>
         <tld/>igaj admonoj.
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="kviet.0igi">kvietigi</ref>
     </refgrp>
     <trd lng="he">&lamed;&yod;<ind>&yod;&shin;&bet;</ind><klr tip="amb">
     (&mem;&het;&lamed;&vav;&qof;&tav;)</klr></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ib;&c_r;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_ib;&c_r;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_m;&c_ib;&c_r;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_ib;&c_r;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">pacificar</trd>
  <trdgrp lng="cs">
    <trd>sm&iacute;&rcaron;it</trd>,
    <trd>usm&iacute;&rcaron;it</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>befrieden</trd>,
    <trd>vers&ouml;hnen</trd>,
    <trd>beruhigen</trd>
  </trdgrp>
  <trd lng="en">pacify</trd>
  <trdgrp lng="es">
    <trd>pacificar</trd>,
    <trd>apaciguar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>apaiser</trd>,
    <trd>pacifier</trd>
  </trdgrp>
  <trd lng="he">&lamed;&yod;<ind>&yod;&shin;&bet;</ind><klr tip="amb">
    (&mem;&het;&lamed;&vav;&qof;&tav;)</klr></trd>
  <trdgrp lng="hu">
    <trd>b&eacute;k&iacute;t</trd>,
    <trd>kib&eacute;k&iacute;t</trd>,
    <trd>megb&eacute;k&iacute;t</trd>
  </trdgrp><trd lng="hu">lecsendes&iacute;t</trd>
  <trd lng="id">[<ind>damai</ind>] mendamaikan</trd>
  <trdgrp lng="it">
    <trd>far fare la pace</trd>, 
    <trd>pacificare</trd>, 
    <trd>appacificare</trd>
  </trdgrp>
  <trd lng="nl">verzoenen</trd>
  <trdgrp lng="pl">
    <trd>pacyfikowa&cacute;</trd>,
    <trd>pogodzi&cacute;</trd>,
    <trd>uspokaja&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>pacifica</trd>,
    <trd>reconcilia</trd>,
    <trd>potoli</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_i;&c_r;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_m;&c_i;&c_r;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_i;&c_r;&c_i;&c_t;&c_mol;</trd>
