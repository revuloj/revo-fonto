<drv mrk="pac.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="pac.0a.KOMUNE">
    <dif>
      Estanta en <tld/>o:
      <ekz>
        severa netu&scirc;ebleco de nia fundamento estas <klr>[...]</klr>
        kondi&ccirc;o por nia regula kaj <tld/>a progresado
        <fnt><bib>F</bib>, <lok>anta&ubreve;parolo</lok></fnt>;
      </ekz>
      <ekz>
        tiuj homoj estas <tld/>aj kun ni
        <fnt><bib>MT</bib>, <lok>&Gen; 34:21</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>aj kunvenoj;
      </ekz>
      <ekz>
        <tld/>e diskutadi.
      </ekz>
    </dif>
    <trd lng="sv">fredlig</trd>
    <trd lng="fr">pacifique<klr tip="amb"> (adj.)</klr></trd>
  </snc>
  <snc mrk="pac.0a.PSI">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ne malkvietiganta la animon:
      <ekz>
        nokto sankta, nokto <tld/>a
        <fnt>
          <vrk><url ref="https://lernu.net/ru/forumo/temo/394">Nokto Sankta, Nokto Paca</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a silento, ke ne blovas e&ccirc; vento
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        estis trankvile kaj <tld/>e en la buduareto
        <fnt><bib>FK</bib>, <lok>la porcio da glacia&jcirc;o</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kviet.0a.paciga">kvieta<sncref/></ref>
    </refgrp>
    <trd lng="sv">fridfull</trd>
    <trd lng="fr">paisible</trd>
  </snc>
  <snc mrk="pac.0a.pacfavora">
    <dif>
      Celanta <tld/>on:
      <ekz>
        nia diligenta kolegaro en laboro <tld/>a ne laci&gcirc;os
        <fnt>
          <aut>L. L. Zamenhof</aut>
          <vrk>La Espero</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="he">&shin;&vav;&het;&resh; &shin;&lamed;&vav;&fmem;</trd>
  <trd lng="fr">pacifique<klr tip="amb"> (adj.)</klr></trd>
  </snc>
