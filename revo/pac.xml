<drv mrk="pac.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="pac.0a.KOMUNE">
    <dif>
      Estanta en <tld/>o:
      <ekz>
        severa netu&scirc;ebleco de nia fundamento estas <klr>[...]</klr> 
        kondi&ccirc;o por nia regula kaj <tld/>a progresado 
        <fnt><bib>F</bib>, <lok>anta&ubreve;parolo</lok></fnt>;
      </ekz>
      <ekz>
        tiuj homoj estas <tld/>aj kun ni 
        <fnt><bib>MT</bib>, <lok>&Gen; 34:21</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>aj kunvenoj; 
      </ekz>
      <ekz>
        <tld/>e diskutadi. 
      </ekz>
    </dif>
    <trd lng="sv">fredlig</trd>
    <trd lng="fr">pacifique<klr tip="amb"> (adj.)</klr></trd>
  </snc>
  <snc mrk="pac.0a.PSI">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ne malkvietiganta la animon:
      <ekz>
        nokto sankta, nokto <tld/>a
        <fnt>
          <vrk><url ref="https://lernu.net/ru/forumo/temo/394">Nokto Sankta, Nokto Paca</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a silento, ke ne blovas e&ccirc; vento
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        estis trankvile kaj <tld/>e en la buduareto 
        <fnt><bib>FK</bib>, <lok>la porcio da glacia&jcirc;o</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kviet.0a.paciga">kvieta<sncref/></ref>
    </refgrp>
    <trd lng="sv">fridfull</trd>
    <trd lng="fr">paisible</trd>
  </snc>
  <snc mrk="pac.0a.pacfavora">
    <dif>
      Celanta <tld/>on:
      <ekz>
        nia diligenta kolegaro en laboro <tld/>a ne laci&gcirc;os
        <fnt>
          <aut>L. L. Zamenhof</aut>
          <vrk>La Espero</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="he">&shin;&vav;&het;&resh; &shin;&lamed;&vav;&fmem;</trd>
  <trd lng="fr">pacifique<klr tip="amb"> (adj.)</klr></trd>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_r;&c_n;&c_y;</trd>
  <trd lng="bg">&c_m;&c_i;&c_r;&c_je;&c_n;</trd>
  <trd lng="ca">pac&iacute;fic</trd>
  <trdgrp lng="cs">
    <trd>m&iacute;rn&yacute;</trd>,
    <trd>sm&iacute;rn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>friedlich</trd>,
    <trd>friedvoll</trd>,
    <trd>Friedens-</trd>,
    <trd>still</trd>
  </trdgrp>
  <trd lng="es">pac&iacute;fico</trd>
  <trd lng="en">peaceful</trd>
  <trd lng="he">&shin;&lamed;&vav;</trd>
  <trd lng="hu">b&eacute;k&eacute;s</trd>
  <trd lng="id">damai</trd>
  <trd lng="it">pacifico</trd>
  <trdgrp lng="nl">
    <trd>vredig</trd>,
    <trd>vreedzaam</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pokojowy</trd>,
    <trd>spokojny</trd>
  </trdgrp>
  <trd lng="ro">pa&#537;nic</trd>
  <trd lng="ru">&c_m;&c_i;&c_r;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>mierumilovn&yacute;</trd>,
    <trd>pokojamilovn&yacute;</trd>,
    <trd>pokojn&yacute;</trd>,
    <trd>zn&aacute;&scaron;anliv&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_i;&c_r;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_s;&#x43F;&#x43E;&#x43A;&c_ib;&c_j;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

