<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$"> <kap>
  <ofc>*</ofc><rad>pac</rad>/o
</kap>
<drv mrk="pac.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Stato de nacio a&ubreve; tribo, kiu ne estas militanta
kontra&ubreve; alia:
      <ekz>
        se vi volas <tld/>on, preparu <tld/>on;
      </ekz>
      <ekz>
        en tempo de <tld/>o;
      </ekz>
      <ekz>
        la artoj de la <tld/>o;
      </ekz>
      <ekz>
        armita <tld/>o
        <klr>(dum kiu &ccirc;iu sin pretigas por milito)</klr>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Stato de anaro (nacio, gento, familio, societo, kunveno), inter
kies
      anoj ekzistas nenia malakordo nek malkonsento:
      <ekz>
        vivi en <tld/>o;
      </ekz>
      <ekz>
        ili foriris de li en <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la <tld/>o regis;
      </ekz>
      <ekz>
        <ind><tld/>o al lia cindro</ind>
        (<ref tip="vid" cel="ripoz.0i">ripozi</ref>)
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">b&eacute;ke poraira</trd>        
      </ekz>
      <ekz>
        ka&ubreve;zi<fnt>Z</fnt>, semi<fnt>Z</fnt>, disvastigi
        mal<tld/>on
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="akord.0o">akordo</ref>,
      <ref cel="konkor.0o">konkordo</ref>,
      <ref cel="konsen.0o">konsento</ref>,
      <ref cel="harmon.0o">harmonio</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_r;</trd>
  <trd lng="ca">pau</trd>
  <trd lng="cs">m&iacute;r</trd>
  <trd lng="de">Friede</trd>
  <trd lng="he">&shin;&lamed;&vav;&fmem;</trd>
  <trd lng="en">peace</trd>
  <trd lng="fr">paix</trd>
  <trd lng="es">paz</trd>
  <trd lng="hu">b&eacute;ke</trd>
  <trd lng="nl">vrede</trd>
  <trd lng="ru">&c_m;&c_i;&c_r; <klr
tip="amb">(&c_o;&c_t;&c_s;&c_u;&c_t;&c_s;&c_t;&c_v;&c_i;&c_je;
&c_v;&c_o;&c_j;&c_n;&c_y;)</klr></trd>
  <trd lng="vo">p&uuml;d</trd>
</drv>

<drv mrk="pac.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Estanta en <tld/>o:
      <ekz>
        ili estas <tld/>aj kun ni<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>aj kunvenoj;
      </ekz>
      <ekz>
        <tld/>e diskutadi.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Celanta <tld/>on:
      <ekz>
        ni ne laci&gcirc;os en nia <tld/>a laboro<fnt>Z</fnt>.
      </ekz>
    </dif>
      <trd lng="he">&shin;&vav;&het;&resh;
&shin;&lamed;&vav;&fmem;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ne malkvietiganta la animon:
      <ekz>
        <tld/>a nokto;
      </ekz>
      <ekz>
        en la <tld/>a silento ne blovas e&ccirc; vento<fnt>Z</fnt>;
      </ekz>
      <ekz>
        estis trankvile kaj <tld/>e en la buduaro<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="seren.0a">serena</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_r;&c_n;&c_y;</trd>
  <trd lng="ca">pac&iacute;fic</trd>
  <trd lng="de">friedlich</trd>
  <trd lng="es">pac&iacute;fico</trd>
  <trd lng="he">&shin;&lamed;&vav;</trd>
  <trd lng="hu">b&eacute;k&eacute;s</trd>
  <trdgrp lng="nl">
    <trd>vredig</trd>,
    <trd>vreedzaam</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_i;&c_r;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="pac.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari, ke iu a&ubreve; io restu <tld/>a, akordigi:
      <ekz>
        li <tld/>igos viajn
        malamikojn kun vi<fnt>Z</fnt>;
      </ekz>
      <ekz>
        jen estas la sola rimedo por <tld/>igi E&ubreve;ropon;
      </ekz>
      <ekz>
        venas <tld/>i&gcirc;o post mal<tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>igi la logikon kun la tradicio<fnt>Z</fnt>,
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la sa&gcirc;on kun la rimo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi ne povas <tld/>i&gcirc;i kun tiu ideo.
      </ekz>
    </dif>
    <trd lng="he">&lamed;&he;&shin;&lamed;&yod;&fmem;
\<ind>&shin;&lamed;&fmem;</ind>\</trd>
  </snc>
  <trdgrp lng="hu">
    <trd>b&eacute;k&iacute;t</trd>
    <trd>kib&eacute;k&iacute;t</trd>,
    <trd>megb&eacute;k&iacute;t</trd>
  </trdgrp>
  <snc>
    <dif>
      Fari ke iu a&ubreve; io estu denove <tld/>a:
      <ekz>
        <tld/>igi
        mal<tld/>on<fnt>Z</fnt>,
      </ekz>
      <ekz>
        malkonsenton, ribelon, disputon;
      </ekz>
      <ekz>
        <tld/>igaj admonoj
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kviet.0igi">kvietigi</ref>.
    </refgrp>
    <trd lng="he">&lamed;&yod;<ind>&yod;&shin;&bet;</ind><klr tip="amb">
(&mem;&het;&lamed;&vav;&qof;&tav;)</klr></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ib;&c_r;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_ib;&c_r;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_m;&c_ib;&c_r;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_ib;&c_r;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">pacificar</trd>
  <trd lng="de">befrieden</trd>
  <trd lng="es">pacificar</trd>
  <trd lng="hu">lecsendes&iacute;t</trd>
  <trd lng="nl">verzoenen</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_i;&c_r;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_m;&c_i;&c_r;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_i;&c_r;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="pac.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="pacifi.0o">Pacifismo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_c;&c_y;&c_f;&c_ib;&c_z;&c_m;</trd>
  <trd lng="ca">pacifisme</trd>
  <trd lng="de">Pazifismus</trd>
  <trd lng="es">pacifismo</trd>
  <trd lng="he">&pe;&tsadi;&yod;&pe;&yod;&zayin;&fmem;</trd>
  <trd lng="hu">pacifizmus</trd>
  <trd lng="nl">pacifisme</trd>
  <trd lng="ru">&c_p;&c_a;&c_c;&c_i;&c_f;&c_i;&c_z;&c_m;</trd>
</drv>

<drv mrk="pac.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="pacifi1.0o">Pacifisto</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Pazifist</trd>,
    <trd>Friedensfreund</trd>
  </trdgrp>
  <trd lng="be">&c_p;&c_a;&c_c;&c_y;&c_f;&c_ib;&c_s;&c_t;</trd>
  <trd lng="ca">pacifista</trd>
  <trd lng="es">pacifista</trd>
  <trdgrp lng="he">
    <trd>&shin;&vav;&het;&resh; &shin;&lamed;&vav;&fmem;</trd>,
    <trd>&pe;&tsadi;&yod;&pe;&yod;&samekh;&tet;</trd>
  </trdgrp>
  <trd lng="hu">pacifista</trd>
  <trd lng="nl">pacifist</trd>
  <trd lng="ru">&c_p;&c_a;&c_c;&c_i;&c_f;&c_i;&c_s;&c_t;</trd>
</drv>

<drv mrk="pac.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti en mal<tld/>a stato:
      <ekz>
        pro limoj kaj baroj mal<tld/>as
        najbaroj
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        pli bone estas havon fordoni ol kun homoj mal<tld/>i
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        konsento konstruas, mal<tld/>o detruas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        mal<tld/>ema virino estas kiel konstanta gutado<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mal<tld/>i&gcirc;inta frato estas pli obstina ol fortikigita
        urbo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_v;&c_a;&c_r;&c_y;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_a;&c_r;&c_a;&c_g;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="he">&lamed;<ind>&resh;&yod;&bet;</ind></trd>
  <trd lng="hu">b&eacute;k&eacute;tlenkedik</trd>  
  <trd lng="nl">onenigheid hebben</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_s;&c_o;&c_r;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_r;&c_a;&c_zh;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
