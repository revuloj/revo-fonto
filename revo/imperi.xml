  </trdgrp>
  <trd lng="uk">&c_ib;&#x43C;&#x43F;&c_je;&c_r;&c_s;&#x44C;&#x43A;&c_i;&c_j;</trd>
</drv>

<drv mrk="imperi.0i">
  <kap><tld/>i</kap>
