<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>imperi</rad>/o
</kap>
<drv mrk="imperi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="imperi.0o.monarkio">
    <dif>
      Granda plurnacia regno, kiu povas esti bazita sur militforto,
      ideologio a&ubreve; dinastia principo, kutime
      <ref tip="super" cel="monarh1.0o.POL">monarkio</ref>
      kies estro sin rigardas supera al la re&gcirc;oj:
      <ekz>
        la roma, &ccirc;ina, germana, a&ubreve;stra-hungara <tld/>o;
      </ekz>
      <ekz>
        la napoleona <tld/>o.
      </ekz>
    </dif>
    <trd lng="nl">rijk</trd>
  </snc>
  <snc mrk="imperi.0o.monarkio.koloniisma">
    <dif>
      La tuto de granda regno kun siaj kolonioj:
      <ekz>
        la franca, brita <tld/>o;
      </ekz>
      <ekz>
        akiri al si gravan kolonian <tld/>on.
      </ekz>
    </dif>
    <trd lng="nl">keizerrijk</trd>
  </snc>
  <snc mrk="imperi.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Granda kaj kutime supernacia kompanio:
      <ekz>
        la <tld/>o de Majkrosoft.
      </ekz>
    </dif>
    <ref tip="vid" cel="regn.0o.FIG">regno</ref>
    <trd lng="nl">imperium</trd>
  </snc>
  <trdgrp lng="de">
    <trd>Reich</trd>,
    <trd>Imperium</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>empire<klr> (politique)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_m;&c_p;&c_je;&c_r;&c_i;&c_ja;</trd>
</drv>

<drv mrk="imperi.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Apartenanta al <tld/>o:
      <ekz>
        <tld/>a lando, kolonio, krono, titolo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">imperial</trd>
  <trdgrp lng="fr">
    <trd>imp&eacute;rial</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_m;&c_p;&c_je;&c_r;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="imperi.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Regi, kiel <tld/>o; superpotenci:
      <ekz>
        dividu kaj <tld/>u!
      </ekz>
      <ekz>
        la romana <tld/>ado iam etendi&gcirc;is super Anglujo.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">heersen</trd>
</drv>

<drv mrk="imperi.0estro">
  <kap><tld/>estro</kap>
  <snc>
    <dif>
      Plej alta rango de <ref tip="super" cel="monarh.0o">monar&hcirc;o</ref>:
      <ekz>
        la Re&gcirc;o de Granda Britujo kaj <tld/>estro de Hindujo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Kaiser</trd>
  <trdgrp lng="fr">
    <trd>empereur</trd>
  </trdgrp>
  <trd lng="nl">keizer</trd>
  <trd lng="ru">&c_i;&c_m;&c_p;&c_je;&c_r;&c_a;&c_t;&c_o;&c_r;</trd>
</drv>

<drv mrk="imperi.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="imperi.0ismo.supernacia">
    <dif>
      Supernacia ideologio a&ubreve; alia fenomeno servanta kiel bazo
      de <tld/>o:
      <ekz>
        la <tld/>ismo de la
        <ind>Sankta Roma <tld lit="I"/>o &Gcirc;ermana</ind>
        apelaciis al kristanismo kaj historia modelo;
        <trd lng="en">Holy Roman Empire</trd>
        <trd lng="de">Heiliges R&ouml;misches Reich Deutscher Nation</trd>
        <trd lng="fr">Saint Empire Romain Germanique</trd>
        <trd lng="nl">Heilig Roomse Rijk der <ind>Duitse</ind> natie</trd>
        <trd lng="ru">&c_S;&c_v;&c_ja;&c_shch;&c_je;&c_n;&c_n;&c_a;&c_ja; &c_R;&c_i;&c_m;&c_s;&c_k;&c_a;&c_ja; &c_i;&c_m;&c_p;&c_je;&c_r;&c_i;&c_ja;
        &c_G;&c_je;&c_r;&c_m;&c_a;&c_n;&c_s;&c_k;&c_o;&c_j; &c_N;&c_a;&c_c;&c_i;&c_i;</trd>
      </ekz>
      <ekz>
        la artefariteco kondi&ccirc;as la sennaciigon de nia planedo
        ... se morga&ubreve; E&ubreve;ropo kaj Usono estos regataj socialisme, la
        sennaciiga procezo ne povos pro tio halti kaj sekve ni havos
        ian socialisman <tld/>ismon
        <fnt>
        <aut>Lanti</aut>:
        <vrk>&Ccirc;u ili estas sennaciistoj?</vrk>.</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="imperi.0ismo.naciisma">
    <dif>
      Doktrino, agado de tiuj, kiuj celas por sia patrujo
      superregadon super fremdaj landoj; mondregemo.
    </dif>
    <refgrp tip="sub">
      <ref cel="fasxis.0o">fa&scirc;ismo</ref>.
    </refgrp>
  </snc>
  <snc mrk="imperi.0ismo.MAH">
    <uzo tip="fak">MAH</uzo>
    <dif>
      Lasta evolustadio de la kapitalismo.
    </dif>
  </snc>
  <trd lng="de">Imperialismus</trd>
  <trdgrp lng="fr">
    <trd>imp&eacute;rialisme</trd>
  </trdgrp>
  <trd lng="nl">imperialisme</trd>
  <trd lng="ru">&c_i;&c_m;&c_p;&c_je;&c_r;&c_i;&c_a;&c_l;&c_i;&c_z;&c_m;</trd>
</drv>

<drv mrk="imperi.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Partiano de <tld/>ismo; <tld/>ismano.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Imperialist</trd>,
    <trd>Kriegstreiber</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>imp&eacute;rialiste<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="nl">imperialist</trd>
  <trd lng="ru">&c_i;&c_m;&c_p;&c_je;&c_r;&c_i;&c_a;&c_l;&c_i;&c_s;&c_t;</trd>
</drv>
</art>
