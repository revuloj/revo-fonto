<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kuplet</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>

<drv mrk="kuplet.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="kuplet.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <ref tip="dif" cel="strof.0o">strofo</ref>
    <ekz>
      finante &ccirc;iun <tld/>on, &scirc;i estis levanta tre alte la nudan piedon
      <fnt><bib>Metrop</bib></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_p;&c_l;&c_je;&c_t;</trd>
  <trd lng="de">Couplet</trd>
  <trd lng="en">couplet</trd>
  <trd lng="fr">couplet</trd>
  <trd lng="hu">versszak</trd>
  <trdgrp lng="nl">
    <trd>couplet</trd>,
    <trd>strofe</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_u;&c_p;&c_l;&c_je;&c_t;</trd>
</drv>
</art>

