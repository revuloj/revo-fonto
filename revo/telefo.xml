<drv mrk="telefo.0o">
  <kap><tld/>o</kap>
  <snc mrk="telefo.0o.TEL">
    <uzo tip="fak">TEL</uzo>
    <uzo tip="fak">ELET</uzo>
    <dif>
      Elektra <ref tip="super" cel="aparat.0o.TEK">aparato</ref> ebliganta interparoladon je
      distanco
      per starigo de komunika kanalo inter la partoprenantoj
      de la interparolo:
      <ekz>
        &scirc;i krias el la <tld/>o: la germanoj kapitulacis! Norvegujo estas libera
        <fnt>
          <bib>LGA</bib>
        </fnt>!
      </ekz>
      <ekz>
        <tld/>oj tintis, tintigaj kaj teruraj dira&jcirc;oj ventege pelis unu la alian
        <fnt>
          <bib>HsT</bib>
        </fnt>;
      </ekz>
      <ekz>
        turninte trifoje per la fingro radeton de a&ubreve;tomata <tld/>o, li alvokis
        <nom>Vinokurov</nom>&#8217;on
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi e&ubreve;ropanoj estas sa&gcirc;aj, <klr>[&#8230;]</klr> vi inventis anka&ubreve; vapor&scirc;ipon,
        aeroplanon, a&ubreve;tomobilon, elektran lumon, <tld/>on, telegrafon, radion,
        kinofilmojn kaj multajn aliajn mirinda&jcirc;ojn
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
        </fnt>;
      </ekz>
      <ekz>
        nia lingvo estas kvaza&ubreve; <tld/>aparato, kiu bezonas esti fadenligita kun similaj
        aparatoj por povi utile funkcii
        <fnt>
          <bib>Lanti</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hola.0">halo, hola</ref>,
      <ref cel="line.0o">lineo</ref>,
      <ref cel="telefoni.0o">telefonio</ref>,
      <ref cel="bud.telefon0o">telefonbudo</ref>,
      <ref cel="kart.telefon0o">telefonkarto</ref>,
      <ref cel="kod.telefon0o">telefonkodo</ref>,
      <ref cel="libr.telefon0o">telefonlibro</ref>,
      <ref cel="telefo.0numero">telefonnumero</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_l;&c_je;&c_f;&c_o;&c_n;</trd>
  <trd lng="bg">&c_t;&c_je;&c_l;&c_je;&c_f;&c_o;&c_n;</trd>
  <trdgrp lng="de">
    <trd>Telefon</trd>,
    <trd>Fernsprecher</trd>
  </trdgrp>
  <trdgrp lng="en"><trd>phone</trd>, <trd>telephone</trd></trdgrp>
  <trd lng="es">tel&eacute;fono</trd>
  <trd lng="fr">t&eacute;l&eacute;phone</trd>
  <trdgrp lng="hu">
    <trd>telefon</trd>,
    <trd>t&aacute;vbesz&eacute;l&odblac;</trd>
  </trdgrp>
  <trd lng="id">telepon</trd>
  <trd lng="nl">telefoon</trd>
  <trdgrp lng="pl">
    <trd>telefon</trd>,
    <trd>aparat <ind>telefoniczny</ind></trd>
  </trdgrp>
  <trd lng="pt">telefone</trd>
  <trd lng="ru">&c_t;&c_je;&c_l;&c_je;&c_f;&c_o;&c_n;</trd>
  <trd lng="sv">telefon</trd>
  <trd lng="sk">telef&oacute;n</trd>
  <trd lng="uk">&c_t;&c_je;&#x43B;&c_je;&c_f;&#x43E;&#x43D;</trd>
  <trd lng="zh">&#30005;&#x8BDD; <klr>(tr. &#x96FB;&#x8A71;)</klr> [<ind>di&agrave;nhu&agrave;</ind>]</trd>
  <trdgrp lng="sk">
    <trd>po&scaron;ta</trd>,
    <trd>telef&oacute;n</trd>,
    <trd>telegraf</trd>
  </trdgrp>
  <trd lng="cs">telefon</trd>
</drv>

<drv mrk="telefo.0a">
  <kap>
    <tld/>a
  </kap>
  <snc mrk="telefo.0a.rilata">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        mia <tld/>a sistemo estas eksterordinare kompleksa
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato1/monato-9401-telefona-labirinto.txt">Will Green kondukas vin
          tra sia</url></vrk>
          <lok>94/01</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>po&scirc;<tld/>a reto</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Steloj;monato2/007052.html">Verda nature, science, socie</url></vrk>
        </fnt>;
        <trd lng="de">Mobilfunknetz</trd>
      </ekz>
    </dif>
  </snc>
  <snc mrk="telefo.0a.per">
    <dif>
      Per <tld/>o:
      <ekz>
        temis pri <tld/>a konsultado pri savi perditan leteron en la komputilo de mia
        avino
        <fnt>
          <bib>Monato</bib>
          <aut>Edmund Grimley Evans</aut>
          <vrk><url ref="&Steloj;monato2/006801.html">Interreto: Uniksulo ekspertas
          Unikodon</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        maldungi 4000 britojn, kiuj respondas tage al dekmiloj da <tld/>aj demandoj, kaj
        ilin anstata&ubreve;igi per skipoj de samrespondeculoj en Barato, &scirc;paros grandajn
        monsumojn
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2004/008223.html">&Ccirc;e-telefone ... kiu?</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">telefonisch</trd>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_l;&c_je;&c_f;&c_o;&c_n;&c_n;&c_y;</trd>
  <trd lng="uk">&c_t;&c_je;&#x43B;&c_je;&c_f;&#x43E;&#x43D;&#x43D;&c_i;&c_j;</trd>
</drv>


<drv mrk="telefo.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="telefo.0i.TEL">
    <dif>
      <ref tip="super" cel="parol.0i.diri">Paroli</ref> per <tld/>o:
      <ekz>
        por pli da informo, mi <tld/>is al la itala konsulejo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/006859.html">Seborgo: Urbeto a&ubreve; sendependa
          lando?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        po&scirc;<tld/>o <klr>[ebligas]</klr> <tld/>i &ccirc;iam kaj &ccirc;ie
        <fnt>
          <bib>Monato</bib>
          <aut>Astrid Hanke</aut>
          <vrk><url ref="&Steloj;monato2/007153.html">La benoj de po&scirc;telefono</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>is al mi en <nom>Zagreb</nom> iu vira vo&ccirc;o, radioamatoro, kiu diris, ke li
        &leftquot;havas
        <nom>Alma</nom> el Sarajevo je sia linio&rightquot;
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato1/monato-9312-bosnio.txt">Bosnio</url></vrk>
          <lok>93/12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_e;&c_l;&c_je;&c_f;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_v;&c_a;&c_n;&c_ib;&c_c;&c_mol; &c_p;&c_a; &c_t;&c_e;&c_l;&c_je;&c_f;&c_o;&c_n;&c_je;</trd>
