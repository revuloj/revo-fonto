    <trd>&c_g;&c_o;&c_v;&c_o;&c_r;&c_ja; &c_p;&c_o; &c_t;&c_je;&c_l;&c_je;&c_f;&c_o;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&t_kha;&t_tsheg;&t_pa;&t_ra;&t_tsheg;&t_ga;&t_ta;&t_naro;&t_nga;&t_tsheg;</trd>
