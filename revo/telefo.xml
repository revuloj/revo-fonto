    po&scirc;<tld/>o <fnt><bib>HejmVort</bib></fnt>,
    <var><kap>&ccirc;el<tld/>o</kap></var>,
    <var><kap>sendrata <tld/>o</kap></var>
  </kap>
  <snc mrk="telefo.posx0o.TEL">
    <uzo tip="fak">TEL</uzo>
    <dif>
      Portebla sen&scirc;nura <tld/>o:
      <ekz>
        la po&scirc;<tld/>a reto <klr>[&#8230;]</klr> transportas nur datumojn
        <fnt><aut>Pejno S.</aut>:
        <vrk><url ref="&Monato;/007219.html">Prefere pakete</url></vrk>,
        <bib>Monato</bib>,
        <lok>2001:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;el<tld/>o devus plifaciligi la kontakton al aliaj homoj
        <fnt>
          <aut>A. Oberndorfer</aut>,
          <vrk><url ref="http://peranto.posterous.com/celtelefonoj-malakcelas-konversaciojn">
          &Ccirc;eltelefonoj malakcelas konversaciojn</url></vrk>,
          <lok>Esperanta retradio, 2012-10-12</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia familio donacis al mi <klr>[&#8230;]</klr> sendratan <tld/>on
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato1/monato-9401-telefona-labirinto.txt">Will Green kondukas vin
          tra sia</url></vrk>
          <lok>94/01</lok>
