<drv mrk="telefo.0ejo">
  <kap>
    <tld/>ejo
  </kap>
  <snc>
    <dif>
      Telefonbudo:
      <ekz>
        li jam staris en la plej proksima <tld/>ejo kaj <tld/>is al sia kolego
        <fnt>
          <bib>LGA</bib>
        </fnt>;
      </ekz>
      <ekz>
        li tuj eniris la hotelan <tld/>ejon
        <fnt>
          <bib>TAA</bib>
        </fnt>;
      </ekz>
      <ekz>
        ni eniris <tld/>ejon por elekti adreson <klr>[&#8230;]</klr>, arkitekton, ne estis
        malfacile trovi sur la urbokarto, kie estas la strato
        <fnt>
          <bib>KKr</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_l;&c_je;&c_f;&c_o;&c_n;&c_n;&c_a;&c_ja; &c_b;&c_u;&c_d;&c_k;&c_a;</trd>
  <trd lng="de">Telefonzelle</trd>
  <trd lng="en">phone booth</trd>
  <trd lng="ja">&#x96FB;&#x8A71;&#x5BA4;  <pr>&#12391;&#12435;&#x308F;&#12375;&#12388;</pr></trd>
  <trd lng="nl">telefooncel</trd>
  <trdgrp lng="pl">
    <trd>budka <ind>telefoniczna</ind></trd>,
    <trd>stacja <ind>telefoniczna</ind></trd>
  </trdgrp>
</drv>

<drv mrk="telefo.0isto">
  <kap>
    <tld/>isto
  </kap>
  <snc mrk="telefo.0isto.TEL">
    <uzo tip="fak">TEL</uzo>
    <dif>
      <klr>(anta&ubreve; a&ubreve;tomata perado)</klr> Homo, kiu akceptas vokanton kaj konektas kun la dezirata
      numero:
      <ekz>
        &ccirc;i tie &gcirc;is la mateno kriis <tld/>istoj, &ccirc;iuminute elkuradis kurieroj kaj knaris
        per plumoj skribistoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;<nac>mil deux cent six? bien, merci,</nac>&rightquot; diris la <tld/>istino
        <fnt>
          <bib>TAA</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_l;&c_je;&c_f;&c_a;&c_n;&c_ib;&c_s;&c_t;</trd>
