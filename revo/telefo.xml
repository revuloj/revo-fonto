<drv mrk="telefo.0ejo">
  <kap>
    <tld/>ejo
  </kap>
  <snc>
    <dif>
      Telefonbudo:
      <ekz>
        li jam staris en la plej proksima <tld/>ejo kaj <tld/>is al sia kolego
        <fnt>
          <bib>LGA</bib>
        </fnt>;
      </ekz>
      <ekz>
        li tuj eniris la hotelan <tld/>ejon
        <fnt>
          <bib>TAA</bib>
        </fnt>;
      </ekz>
      <ekz>
        ni eniris <tld/>ejon por elekti adreson <klr>[&#8230;]</klr>, arkitekton, ne estis
        malfacile trovi sur la urbokarto, kie estas la strato
        <fnt>
          <bib>KKr</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_l;&c_je;&c_f;&c_o;&c_n;&c_n;&c_a;&c_ja; &c_b;&c_u;&c_d;&c_k;&c_a;</trd>
  <trd lng="de">Telefonzelle</trd>
  <trd lng="en">phone booth</trd>
</drv>

<drv mrk="telefo.0isto">
  <kap>
    <tld/>isto
  </kap>
  <snc mrk="telefo.0isto.TEL">
    <uzo tip="fak">TEL</uzo>
    <dif>
      <klr>(anta&ubreve; a&ubreve;tomata perado)</klr> Homo, kiu akceptas vokanton kaj konektas kun la dezirata
      numero:
      <ekz>
        &ccirc;i tie &gcirc;is la mateno kriis <tld/>istoj, &ccirc;iuminute elkuradis kurieroj kaj knaris
        per plumoj skribistoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;<nac>mil deux cent six? bien, merci,</nac>&rightquot; diris la <tld/>istino
        <fnt>
          <bib>TAA</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_l;&c_je;&c_f;&c_a;&c_n;&c_ib;&c_s;&c_t;</trd>
  <trd lng="de">Telefonist</trd>
  <trd lng="en">switchboard operator</trd>
  <trd lng="fr">t&eacute;l&eacute;phoniste</trd>
  <trd lng="uk">&c_t;&c_je;&#x43B;&c_je;&c_f;&#x43E;&#x43D;&c_ib;&c_s;&c_t;</trd>
</drv>

<drv mrk="telefo.al0ado">
  <kap>
    al<tld/>ado,
    <var><kap><tld/>alvoko</kap></var>,
    <var><kap><tld/>voko</kap></var>
  </kap>
  <snc mrk="telefo.al0ado.TEL">
    <uzo tip="fak">TEL</uzo>
    <dif>
      Unuopa konekti&gcirc;o de <tld/>oj kaj sekva
      <tld/>a interparolo:
      <ekz>
        &scirc;i timis ke &scirc;i povus maltrafi lian <tld/>alvokon
        <fnt>
          <aut>S. &Scaron;timec</aut>
          <vrk>Ombro sur interna pejza&gcirc;o</vrk>
          <lok>1984</lok>
        </fnt>;       
      </ekz>
      <ekz>
        li estis minacata de anonimaj <tld/>vokoj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/006909.html">Japanio: Takasugi Sinsaku: Novjara kla&ccirc;o super
          konjako</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li restis hejme, por atendi al<tld/>adon de iu el tiuj
        institucioj
        <fnt>
          <aut>Liu Yichang</aut>
          <vrk><url ref="&CRI;1/2005/03/11/1@22006.htm">
          Erare!</url></vrk>
          <lok>&Ccirc;ina Radio Internacia, 2005-03-11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_v;&c_a;&c_n;&c_o;&c_k; <klr>(&c_t;&c_e;&c_l;&c_je;&c_f;&c_o;&c_n;&c_n;&c_y;)</klr></trd>,
    <trd>&c_t;&c_e;&c_l;&c_je;&c_f;&c_a;&c_n;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="en">phone call</trd>
  <trd lng="fr">appel t&eacute;l&eacute;phonique</trd>
  <trd lng="id"><ind>panggil</ind>an telepon</trd>
</drv>

<drv mrk="telefo.lerta0o">
  <kap>lerta <tld/>o,
  <var><kap>inteligenta <tld/>o</kap></var></kap>
  <snc mrk="telefo.lerta0o.TEL">
    <uzo tip="fak">TEL</uzo>
    <dif>
      Po&scirc;telefono provizita per telekomputaj
      kapabloj, fotilo a&ubreve; aliaj funkcioj;
      <ref tip="sin" cel="kompufon.0o.TEL">kompufono</ref>:
      <ekz>
        populari&gcirc;o de la lertaj <tld/>oj
        <fnt>
          <aut>P. Peeraerts</aut>,
          <vrk>en: <url
          ref="http://www.liberafolio.org/2010/elektronikaj-libroj-enmerkatigas-en-esperantujo">
          Elektronikaj libroj enmerkati&gcirc;as en Esperantujo</url></vrk>,
          <lok>libera folio, 2010-03-10</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;ia sistemo estos integrita kun la inteligentaj <tld/>oj
        <fnt>
          <aut>Wang Yin, Zhang Shuangm Wang Lihua</aut>,
          <vrk><url ref="http://www.espero.com.cn/se/txt/2013-01/24/content_514205.htm">
          Beidou -- nova elekto por la tergloba navigada satelita
          sistemo en &Ccirc;inio</url></vrk>,
          <lok>Espero.com.cn, 2013-01-24</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_m;&c_a;&c_r;&c_t;&c_f;&c_o;&c_n;</trd>
  <trdgrp lng="de">
    <trd>Smartphone</trd>,
    <trd>Smartfon</trd>,
    <trd>Schlaufon<klr> [ugs.]</klr></trd>
  </trdgrp>
  <trd lng="en">smartphone</trd>
  <trd lng="fr">t&eacute;l&eacute;phone intelligent</trd>
  <trdgrp lng="id">
    <trd><ind>ponsel</ind> pintar</trd>
    <trd><ind>telepon</ind> pintar</trd>,
  </trdgrp>
</drv>

<drv mrk="telefo.posx0o">
