    <trd>&c_d;&c_z;&c_v;&#x43E;&#x43D;&c_i;&c_t;&c_i;/&c_g;&#x43E;&c_v;&#x43E;&c_r;&c_i;&c_t;&c_i; &#x43F;&#x43E; &c_t;&c_je;&#x43B;&c_je;&c_f;&#x43E;&#x43D;&c_u;</trd>,
