<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc><rad>Paragvaj</rad>/o
</kap>

<drv mrk="paragvaj.P0o">
  <kap><ofc>8</ofc><tld/>o
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/aktoj/aktoj2/8oa.html"> 8a
Oficiala Aldono</url></vrk>, <lok>II.C.1</lok></fnt>
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>
   <fnt><bib>Viki</bib>, <lok><url 
   ref="http://eo.wikipedia.org/wiki/Paragvajo">Paragvajo</url></lok></fnt>
  </kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      Sudamerika <ref tip="super" cel="sxtat.0o">&scirc;tato</ref>,
limanta
      al 
      <ref tip="vid" cel="brazil.B0o">Brazilo</ref>,
      <ref tip="vid" cel="argentin.A0o">Argentino</ref> kaj 
      <ref tip="vid" cel="bolivi.B0o">Bolivio</ref>.
    </dif>
    </snc>
  <trd lng="be">&c_P;&c_a;&c_r;&c_a;&c_g;&c_v;&c_a;&c_j;</trd>
  <trd lng="bg">&c_P;&c_a;&c_r;&c_a;&c_g;&c_v;&c_a;&c_j;</trd>
  <trd lng="cs">Paraguay <klr>(st&aacute;t)</klr></trd>
  <trd lng="de">Paraguay</trd>
  <trd lng="en">Paraguay</trd>
  <trd lng="fa">&f_p;&a_A;&a_r;&a_A;&f_g;&a_w;&a_y_hamza;&a_h;</trd>
  <trd lng="fr">Paraguay</trd>
  <trd lng="id">Paraguay</trd>
  <trd lng="pl">Paragwaj</trd>
  <trd lng="pt">Paraguai</trd>
  <trd lng="sk">Paraguaj</trd>
</drv>

<drv mrk="paragvaj.0ano">
  <kap><tld lit="p"/>ano,
  <var><kap><tld lit="p"/>anino</kap></var></kap>
  <snc>
    <dif>
    Civitano, civitanino de Paragvajo.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_g;&c_v;&c_a;&c_je;&c_c;</trd>
  <trdgrp lng="fr"> 
    <trd>Paraguayen</trd>
    <trd>Paraguayenne<klr> (subst.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="paragvaj.0a">
  <kap><tld lit="p"/>a</kap>
  <snc>
    <dif>
    Rilata al la &scirc;tato Paragvajo.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_g;&c_v;&c_a;&c_j;&c_s;&c_k;&c_ib;</trd>
  <trdgrp lng="fr"> 
    <trd>paraguayen</trd>
    <trd>paraguayenne<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>
</art>


