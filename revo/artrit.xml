<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>artrit</rad>/o
</kap>
<drv mrk="artrit.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Inflamo de la artikoj.
    </dif>
    <refgrp tip="sin">
      <ref cel="podagr.0o">podagro</ref>,
      <ref cel="reuxma.0o">re&ubreve;matismo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Arthritis</trd>,
     <trd>Gelenkentz&uuml;ndung</trd>
  </trdgrp>
  <trd lng="fr">arthrite</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_t;&c_r;&c_i;&c_t;</trd>,
    <trd>&c_v;&c_o;&c_s;&c_p;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je; &c_s;&c_u;&c_s;&c_t;&c_a;&c_v;&c_o;&c_v;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>inflammation i en led</trd>,
    <trd>gikt</trd>
  </trdgrp>
</drv>
</art>
