<drv mrk="artrit.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Inflamo de la artikoj.
    </dif>
    <refgrp tip="sin">
      <ref cel="podagr.0o">podagro</ref>,
      <ref cel="reuxma.0o">re&ubreve;matismo</ref>.
