<art mrk="$Id: var.xml,v 1.28 2023/10/21 16:50:35 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>var</rad>/o
</kap>
<drv mrk="var.0o">
  <kap><tld/>o</kap>
  <snc mrk="var.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="dif" cel="komerc.0ajxo">Komerca&jcirc;o</ref>:
      <ekz>
        vi havos la okazon konvinki&gcirc;i pri la alta kvalito de fre&scirc;aj <tld/>oj kaj pri la
        modereco de l' prezoj
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
          <lok>La Stranga Butiko</lok>
        </fnt>;
      </ekz>
      <ekz>
        krom tio el la eksterlandaj fabrikoj enfluas malmultekostaj
        <tld/>oj en Japanion, kio des pli suferigas enlandajn
        fabrikojn<fnt><aut>Hori Jasuo</aut>: <vrk>Senlaboreco atakas la
        arkipelagon</vrk>, <bib>Monato</bib><lok>jaro 2002a, numero 4a, p.
        10a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
       <ref cel="artikl.0o">artiklo</ref>,
       <ref cel="produk.0ajxo">produkta&jcirc;o</ref>
    </refgrp>
    <ref tip="vid" cel="stok.0o">stoko</ref>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_v;&c_a;&c_r;</trd>
  <trd lng="bo">&#xF45;&#xF0B;&#xF63;&#xF42;&#xF0B;</trd>
  <trdgrp lng="ca">
    <trd>mercaderia</trd>,
    <trd>article</trd>,
    <trd>producte</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>tovar</trd>,
    <trd>v&yacute;robek</trd>,
    <trd>zbo&zcaron;&iacute; <klr>(1 kus)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ware</trd>,
    <trd>Handelsgut</trd>
  </trdgrp>
  <trd lng="el">&epsilon;&my;&pi;&omikron_ton;&rho;&epsilon;&ypsilon;&my;&alfa;</trd>
  <trdgrp lng="en">
    <trd>commodity</trd>,
    <trd>article of commerce</trd>,
    <trd>article of merchandise</trd>,
    <trd>article of trade</trd>
    <trd>trade good</trd>,
    <trd>ware</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>mercader&iacute;a</trd>,
    <trd>art&iacute;culo</trd>,
    <trd>producto</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>marchandise</trd>
  </trdgrp>
  <trd lng="hu">&aacute;ru</trd>
  <trdgrp lng="ja">
    <trd>&#x5546;&#x54C1;  <pr>&#x3057;&#x3087;&#x3046;&#x3072;&#x3093;</pr></trd>,
    <trd>&#x58F2;&#x308A;&#x7269;  <pr>&#x3046;&#x308A;&#x3082;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>waar <klr tip="ind">(handels-)</klr></trd>,
    <trd>handelswaar</trd>
  </trdgrp>
  <trd lng="pl">towar</trd>
  <trd lng="pt">mercadoria</trd>
  <trd lng="ru">&c_t;&c_o;&c_v;&c_a;&c_r;</trd>
  <trd lng="sk">tovar</trd>
  <trd lng="uk">&#x442;&#x43E;&#x432;&#x430;&#x440;</trd>
</drv>
