<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: abrazi.xml,v 1.10 2023/10/22 12:36:12 revo Exp $">
<kap>
  <rad>abrazi</rad>/i
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="abrazi.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="abrazi.0i.TEK">
    <dif>
      Frot-<ref tip="super" cel="uz.el0i.foruzi">eluzi</ref>,
      skrap-eluzi:
      <ekz>
        ni iru, ni iru! &mdash; li rediris kun malklara vo&ccirc;o
        kaj piednuda ekmar&scirc;is la&ubreve; la <tld/>ita &scirc;tonaro
        <fnt>
          <aut>E. Kumi&ccaron;i&cacute;, trad. S. Rukelj</aut>,
          <vrk><url ref="http://free-vz.t-com.hr/Stanko/Surprizitaj.pdf">
          Surprizitaj nuptofestantoj</url></vrk>,
          <lok>inko, 2001</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="erozi.0i">erozii</ref>,
      <ref cel="triv.0i.KOMUNE">trivi</ref>,
      <ref cel="sxlif.0i.TEK">&scirc;lifi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_s;&c_k;&c_r;&c_a;&c_b;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_mol;&c_c;&c_ib;&c_r;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">vyml&iacute;t</trd>
  <trdgrp lng="de">
    <trd>abschaben</trd>,
    <trd>abschleifen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>abrade</trd>,
    <trd>wear away</trd>
  </trdgrp>
  <trd lng="fr">abraser</trd>
  <trdgrp lng="pl">
    <trd>&sacute;ciera&cacute;</trd>,
    <trd>skroba&cacute;</trd>
  </trdgrp>
  <trd lng="sk">vymlie&tcaron;</trd>
</drv>

<drv mrk="abrazi.0o">
  <kap><tld/>o</kap>
  <snc mrk="abrazi.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Eluzado, deprenado de surfacaj eroj per ripetaj
      frotoj, skrapoj, batetoj:
      <ekz>
        en te&hcirc;niko <tld/>o estas anka&ubreve; ofte uzata
        en farado de diversaj aparatoj kaj iliaj
        detaloj por doni al ili necesan formon
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Abrazio">
          Abrazio</url>, 2012-10-20</lok>
        </fnt>.
      </ekz>
    </dif>
    <subsnc>
      <uzo tip="fak">GEOL</uzo>
      <dif>
        Erozio per frotado de movataj eroj en aero, akvo, glacio.
      </dif>
    </subsnc>
    <subsnc>
      <uzo tip="fak">MED</uzo>
      <dif>
        Forskrapo de histo, &ccirc;u pro vundi&gcirc;o, &ccirc;u por kuraca celo.
      </dif>
    <trd lng="de">Ausschabung</trd>
    </subsnc>
    <ref tip="vid" cel="ablaci.0o">ablacio</ref>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_r;&c_a;&c_z;&c_ib;&c_ja;</trd>
  <trd lng="de">Abrasion</trd>
  <trd lng="en">abrasion</trd>
  <trd lng="fr">abrasion</trd>
  <trd lng="pl">abrazja</trd>
  <trdgrp lng="uk">
    <trd>&c_a;&c_b;&c_r;&c_a;&c_z;&c_ib;&c_ja;</trd>,
    <trd>&c_v;&c_i;&c_s;&c_k;&c_r;&c_ib;&c_b;&c_a;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>
</art>

