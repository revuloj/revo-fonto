<vortaro>
<art mrk="$Id: hidrolog.xml,v 1.6 2019/10/07 16:10:32 revo Exp $">
<kap>
  <rad>hidrolog</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="hidrolog.0o">
  <kap><tld/>o</kap>
  <snc mrk="hidrolog.0o.SCI">
    <dif>
      <ref tip="dif" cel="akv.0osciencisto.SCI">Akvosciencisto</ref>:
      <ekz>
        merkrede alvenis in&gcirc;enieroj, agronomoj, <tld/>oj, topografoj 
        kaj ter-mezuristoj
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>;
      </ekz>
      <ekz>
        <nom>Eginald Schlattner</nom> estas <tld/>o, germanlingva verkisto 
        kaj evangelia pastoro en
        Rumanio
        <fnt>
          <vrk><url ref="https://peoplepill.com/people/eginald-schlattner/"
              >Eginald Schlattner - peoplepill.com</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Hydrologe</trd>
  <trd lng="en">hydrologist</trd>
  <trd lng="fr">hydrologue</trd>
  <trd lng="it">idrologo</trd>
  <trdgrp lng="nl">
    <trd>waterbouwkundige</trd>,
    <trd>hydroloog</trd>
  </trdgrp>
</drv>
</art>

