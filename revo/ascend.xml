<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ascend.xml,v 1.9 2023/09/11 10:28:58 revo Exp $">
<kap>
  <rad>ascend</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="ascend.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="ascend.0i.supreniri">
    <dif>
      <ref tip="dif" cel="ir.supren0i.KOMUNE">Supren iri</ref>,
      <ref tip="dif" cel="supr.al0i.KOMUNE">alsupri</ref>:
      <ekz>
        ni <tld/>as pene kaj fine venas al malgranda ebena&jcirc;o
        <fnt>
          <aut>V. Glazunov</aut>,
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Noveloj/Cxarmo.html">La
          &ccirc;armo de &Ccirc;ielmontaro</url></vrk>,
          <lok>Fonto, numero 37a</lok>
        </fnt>;
      </ekz>
      <ekz>
        muro el libroj <klr>[&#8230;]</klr> ascendis &ccirc;e la kontra&ubreve;a flanko de la &ccirc;ambro
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Sen&ccirc;esa Rakonto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="ascend.0i.spirite">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Spirite atingi pli altan &scirc;tupon:
      <ekz>
        mi strebas volitive / <tld/>i al sublima sfer'
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        homo el skeptikeco a&ubreve; dubemo <tld/>as al &leftquot;dikredo&#x201D;
        <fnt>
          <bib>Monato</bib>
          <aut>Armando Zecchin</aut>
          <vrk><url ref="&Monato;/2007/009187.php">Kiu estas humanisto?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <!-- formon maloftan traduku prefere per formo nacilingve malofta -->
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_y;&c_m;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_u;&c_z;&c_d;&c_y;&c_m;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&#xF60;&#xF5B;&#xF7A;&#xF42;&#xF66;&#xF0B;</trd>
  <trd lng="de">aufsteigen</trd>
  <trd lng="fr">gravir</trd>
  <trd lng="he">&#x5DC;&#x5B7;&#x5E2;&#x5B2;&#x5DC;&#x5D5;&#x5B9;&#x5EA;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_h;&#x43E;&c_d;&c_i;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_ib;&c_d;&#x43D;&c_ib;&#x43C;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>
  </trdgrp>
</drv>
</art>

