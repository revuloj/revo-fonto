<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>buf</rad>/o
</kap>
<drv mrk="buf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la ordo <ref tip="super" cel="buf.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Bufo</trd>).
    </dif>
  </snc>
  <trd lng="de">Kr&ouml;te</trd>
  <trd lng="nl">pad</trd>
  <trd lng="ru">&c_zh;&c_a;&c_b;&c_a;</trd>
  <trd lng="sv">padda</trd>
</drv>
<drv mrk="buf.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo
      <ref tip="super" cel="vost.sen0uloj">senvostuloj</ref>
      (<trd lng="la">Bufonidae</trd>), kun veruka ha&ubreve;to.
    </dif>
  </snc>
  <trd lng="de">Kr&ouml;ten</trd>
</drv>
</art>
