<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: buf.xml,v 1.34 2022/04/26 15:00:33 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>buf</rad>/o <fnt><bib>Fab1</bib></fnt>
</kap>
<drv mrk="buf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="amfibi.0o" lst="voko:amfibioj">Amfibio</ref>,
      <ref tip="malprt" cel="vost.sen0uloj">senvostulo</ref>
      kun horizontalaj pupiloj kaj veruka ha&ubreve;to;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> 
      el la ordo <ref tip="malprt" cel="bufed1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Bufo</trd>):
      <ekz>
        la <tld/>o estis terure abomeninda, granda kaj malseka
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam Elizo subakvi&gcirc;is, unu <tld/>o sidi&gcirc;is en &scirc;iaj haroj, la dua sur &scirc;ia frunto
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la puto sub &scirc;tono sidas <tld/>o
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tri oraj haroj de la diablo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en koton oni vin &jcirc;etos, vi, timemuloj, vi, <tld/>oj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>!
      </ekz>
    </dif>
    <ref tip="vid" cel="ran.0o">rano</ref>
    <bld lok="&WCU;/thumb/f/f8/Kr&ouml;te-Kreta.jpg/320px-Kr&ouml;te-Kreta.jpg" prm="CC BY-SA 2.0 de"><tld/>ino, Kreto, Grekujo
        <fnt>
          <aut>Frente, propra foto</aut>
          <lok><url ref="&WCW;/File:Kr%C3%B6te-Kreta.jpg">&WCW;/File:Kr&ouml;te-Kreta.jpg</url></lok>
        </fnt>
    </bld>    
  </snc>                    
  <trd lng="be">&c_r;&c_a;&c_p;&c_u;&c_h;&c_a;</trd>
  <trd lng="bg">&c_k;&c_r;&c_a;&c_s;&c_t;&c_a;&c_v;&c_a; <ind>&c_zh;&c_a;&c_b;&c_a;</ind></trd>
  <trd lng="br">touseg</trd>
  <trdgrp lng="de">
    <trd>Kr&ouml;te</trd>
  </trdgrp>
  <trd lng="en">toad</trd>
  <trd lng="es">sapo</trd>
  <trd lng="fr">crapaud</trd>
  <trd lng="he">&qof;&resh;&pe;&dalet;</trd>
  <trdgrp lng="hu">
    <trd>varangy</trd>,
    <trd>varangyos b&eacute;ka</trd></trdgrp>
  <trd lng="id">kodok</trd>
  <trdgrp lng="it">
    <trd>bufo</trd>,
    <trd>rospo</trd></trdgrp>
  <trd lng="nl">pad</trd>
  <trd lng="pl">ropucha</trd>
  <trd lng="pt">sapo</trd>
  <trd lng="ru">&c_zh;&c_a;&c_b;&c_a;</trd>
  <trd lng="sv">padda</trd>
  <trd lng="sk">ropucha</trd>
  <trd lng="cs">ropucha</trd>
  <trd lng="uk">&#x440;&#x43E;&#x43F;&#x443;&#x445;&#x430;</trd>
</drv>

</art>
