<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>aberaci</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="aberac.0o">
  <kap><tld/>o</kap>

  <snc mrk="aberac.0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Diferenco inter la &scirc;ajna kaj vera situo de astro.
    </dif>
    <trd lng="de">Abweichung</trd>
    <trdgrp lng="pl">
      <trd>aberracja<klr> astronomiczna</klr></trd>,
      <trd>aberracja<klr> gwiezdna</klr></trd>,
      <trd>pozorne odchylenie<klr> (gwiazdy)</klr></trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>odklon</trd>,
      <trd>odch&yacute;lka</trd>
    </trdgrp>
  </snc>

  <snc mrk="aberac.0o.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
    Malperfekto de imago misformita de optika sistemo.
    </dif>
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Disperso de la lumradioj, kiu kontra&ubreve;staras la
formi&gcirc;on de preciza bildo.
    </dif>
    <trd lng="de">Abbildungsunsch&auml;rfe</trd>
    <trdgrp lng="pl">
      <trd>aberracja<klr> &sacute;wiat&lstroke;a</klr></trd>
      <trd>aberracja<klr> uk&lstroke;adu optycznego (optyczna):
chromatyczna, sferyczna, astygmatyzm, dystorsja, koma, krzywizna
pola</klr></trd>	  
    </trdgrp>
    <trd lng="sk">chyba <klr>(&scaron;o&scaron;ovky)</klr></trd>
  </snc>

  <snc mrk="aberac.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Devio disde la normala tipo.
    </dif>
    <trd lng="pl">ob&lstroke;&eogonek;d</trd>
  </snc>

  <snc mrk="aberac.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Deflanki&gcirc;o disde la normala stato.
      <ekz><tld/>o de la sensoj<fnt><bib>PIV2</bib></fnt></ekz>
      <ekz><tld/>o de la
ju&gcirc;okapablo<fnt><bib>PIV2</bib></fnt></ekz>
      <ekz>genera <tld/>o<fnt><bib>PIV2</bib></fnt></ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>aberracja</trd>,
      <trd>zboczenie</trd>,
      <trd>odchylenie</trd>
    </trdgrp>
  </snc>

  <trd lng="be">&c_a;&c_b;&c_e;&c_r;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trd lng="br">aberradenn</trd>
  <trd lng="de">Aberration</trd>
  <trd lng="en">aberration</trd>
  <trd lng="es">aberraci&oacute;n</trd>
  <trd lng="fr">aberration</trd>
  <trdgrp lng="nl">
    <trd>afwijking</trd>,
    <trd>aberratie</trd>
  </trdgrp>
  <trd lng="hu">aberr&aacute;ci&oacute;</trd>
  <trd lng="pt">aberra&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_a;&c_b;&c_je;&c_r;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
</drv>
</art>
