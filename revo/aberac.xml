<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>aberaci</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="aberac.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">AST</uzo>
    <dif>
      Diferenco inter la &scirc;ajna kaj vera situo de astro.
    </dif>
    <trdgrp lng="sk">
      <trd>odklon</trd>,
      <trd>odch&yacute;lka</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">FOT</uzo>
    <dif>
      Disperso de la lumradioj, kiu kontra&ubreve;staras la formi&gcirc;on de
      preciza bildo.
    </dif>
    <trd lng="sk">chyba <klr>(&scaron;o&scaron;ovky)</klr></trd>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_e;&c_r;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Aberration</trd>,
    <trd>Abirrung</trd>
  </trdgrp>
  <trd lng="en">aberration</trd>
  <trd lng="es">aberraci&oacute;n</trd>
  <trd lng="fr">aberration</trd>
  <trdgrp lng="nl">
    <trd>afwijking</trd>,
    <trd>aberratie</trd>
  </trdgrp>
  <trd lng="hu">aberr&aacute;ci&oacute;</trd>
  <trd lng="pt">aberra&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_a;&c_b;&c_je;&c_r;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
</drv>
</art>
