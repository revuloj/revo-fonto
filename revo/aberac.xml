<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aberac.xml,v 1.37 2023/10/27 12:51:04 revo Exp $">
<kap>
  <rad>aberaci</rad>/i
</kap>
<drv mrk="aberac.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="aberac.0i.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="vid" cel="flank.de0igxi">Deflanki&gcirc;i</ref>, <ref tip="vid" cel="voj.de0igxi">devoji&gcirc;i</ref> disde la normala,
      atendebla stato:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la UEA-lorno plene kontentigas la centran vidon, sed <tld/>as
        &ccirc;e la lensorando
        <fnt>
          <aut>C. Minnaja</aut>,
          <vrk>Lingvo kaj popolo [recenzo]</vrk>,
          <bib>LOdE</bib>,
          <lok>2007:3 (149)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>desviar-se</trd>,
    <trd>esgarriar-se</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>aberovat</trd>,
    <trd>odchylovat se <klr>(&ccaron;o&ccaron;ka, dalekohled, sv&ecaron;tlo)</klr></trd>
  </trdgrp>
  <trd lng="es">aberrar</trd>
  <trd lng="it">aberrare</trd>
  <trdgrp lng="sk">
    <trd>aberova&tcaron;</trd>,
    <trd>odchy&lcaron;ova&tcaron; sa</trd>
  </trdgrp>
</drv>

<drv mrk="aberac.0o">
  <kap><tld/>o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt></kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="aberac.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Deflanki&gcirc;o disde la normala stato:
      <ekz>
        <tld/>o de la sensoj<fnt><bib>PIV2</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>o de la ju&gcirc;okapablo<fnt><bib>PIV2</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">MUZ</uzo>
        <tld/>o je &#x2212;2 cendoj kompare kun la &gcirc;usta kvinto plejofte ne
        &gcirc;enas
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;registro (orgeno)&rightquot;,
        2008-07-10</lok></fnt>.
      </ekz>
    </dif>
    <subsnc mrk="aberac.0o.AST">
      <uzo tip="fak">AST</uzo>
      <dif>
        Diferenco inter la &scirc;ajna kaj vera situo de astro.
      </dif>
      <trd lng="da">afvigelse</trd>
      <trd lng="de">Abweichung</trd>
      <trdgrp lng="pl">
        <trd>aberracja<klr> astronomiczna</klr></trd>,
        <trd>aberracja<klr> gwiezdna</klr></trd>,
        <trd>pozorne odchylenie<klr> (gwiazdy)</klr></trd>
      </trdgrp>
      <trdgrp lng="sk">
        <trd>odklon</trd>,
        <trd>odch&yacute;lka</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="aberac.0o.FIZ">
      <uzo tip="fak">FIZ</uzo>
      <dif>
        Disperso de la lumradioj, kiu kontra&ubreve;staras la
        formi&gcirc;on de preciza bildo.
      </dif>
    </subsnc>
    <subsnc mrk="aberac.0o.FOT">
      <uzo tip="fak">FOT</uzo>
      <dif>
        Malperfekto de bildo misformita de optika sistemo.
      </dif>
      <trd lng="de">Abbildungsunsch&auml;rfe</trd>
      <trdgrp lng="pl">
        <trd>aberracja<klr> &sacute;wiat&lstroke;a</klr></trd>,
        <trd>aberracja<klr> uk&lstroke;adu optycznego</klr></trd>
      </trdgrp>
      <trd lng="sk">chyba <klr>(&scaron;o&scaron;ovky)</klr></trd>
    </subsnc>
    <subsnc mrk="aberac.0o.MED">
      <uzo tip="fak">MED</uzo>
      <dif>
        Devio disde la normala tipo.
      </dif>
      <trd lng="pl">ob&lstroke;&eogonek;d</trd>
    </subsnc>
    <trd lng="zh">&#x50CF;&#x5DEE; <pr>xi&#xE0;ngch&#x101;</pr></trd>
</snc>
  <trd lng="he">&#x5E1;&#x5B0;&#x5D8;&#x5B4;&#x5D9;&#x5B8;&#x5D4;</trd>
  <trdgrp lng="ja">
    <trd>&#x5149;&#x884C;&#x5DEE;  <pr>&#x3053;&#x3046;&#x3053;&#x3046;&#x3055;</pr></trd>,
    <trd>&#x53CE;&#x5DEE;  <pr>&#x3057;&#x3085;&#x3046;&#x3055;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>aberracja</trd>,
    <trd>zboczenie</trd>,
    <trd>odchylenie</trd>
  </trdgrp>
  <trd lng="be">&c_a;&c_b;&c_e;&c_r;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trd lng="br">aberradenn</trd>
  <trd lng="ca">aberraci&oacute;</trd>
  <trd lng="da">aberration</trd>
  <trd lng="de">Aberration</trd>
  <trd lng="en">aberration</trd>
  <trd lng="es">aberraci&oacute;n</trd>
  <trd lng="fa">&a_A;&a_b;&f_y;&a_r;&a_A;&a_h;&f_y;</trd>
  <trd lng="fr">aberration</trd>
  <trdgrp lng="nl">
    <trd>afwijking</trd>,
    <trd>aberratie</trd>
  </trdgrp>
  <trd lng="hu">aberr&aacute;ci&oacute;</trd>
  <trd lng="it">aberrazione</trd>
  <trd lng="pt">aberra&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_a;&c_b;&c_je;&c_r;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="uk">
    <trd>&c_a;&c_b;&c_je;&c_r;&c_a;&c_c;&c_ib;&#x44F;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_h;&c_i;&#x43B;&c_je;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_r;&#x43E;&c_z;&c_s;&c_ib;&#x44E;&c_v;&c_a;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
