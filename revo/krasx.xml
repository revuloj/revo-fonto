<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: krasx.xml,v 1.17 2022/05/01 08:09:25 revo Exp $">
<kap>
  <rad>kra&scirc;</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="krasx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="krasx.0i.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      <ref tip="super" cel="fal.0i.mallevigxi">Fal</ref>egi kaj
      <ref cel="frakas.0igxi">frakasi&gcirc;i</ref>:
      <ekz>
        la flugma&scirc;ino komencis deteri&gcirc;i kaj <tld/>is en alian flugma&scirc;inon, kiu tre
        malfeli&ccirc;e hazarde ankora&ubreve; baris la vojon
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/007143.html">&Ccirc;u ni mortu miskomprene? (2)</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la 16an de julio <tld/>is albana registara helikoptero en Adriatikon
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2006/009052.php">&Ccirc;u akcidento a&ubreve; murdo?</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        pasintjare <tld/>is en Litovio 50 km for de <nom>Kaunas</nom> rusa &ccirc;asaviadilo
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2006/008804.php">Post kra&scirc;o levi&gcirc;as esperoj</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        ma&scirc;ino C-130 de la irana aer-armeo paneis kaj <tld/>is apud 10-eta&gcirc;a lo&gcirc;ejo
        <fnt>
          <bib>Monato</bib>
          <aut>Keyhan Sayadpour</aut>
          <vrk><url ref="&Monato;/2006/008856.php">Embargo kontribuis al katastrofo</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_z;&c_mol;&c_b;&c_ib;&c_c;&c_c;&c_a; <klr tip="amb">(&c_p;&c_r;&c_a;
      &c_s;&c_a;&c_m;&c_a;&c_l;&c_jo;&c_t;)</klr></trd>,
      <trd>&c_p;&c_a;&c_c;&c_ja;&c_r;&c_p;&c_je;&c_c;&c_mol;
      <ind>&c_k;&c_a;&c_t;&c_a;&c_s;&c_t;&c_r;&c_o;&c_f;&c_u;</ind></trd>
    </trdgrp>
    <trdgrp lng="bg">
      <trd>&c_r;&c_a;&c_z;&c_b;&c_i;&c_v;&c_a;&c_m; &c_s;&c_je; (&c_z;&c_a; &c_s;&c_a;&c_m;&c_o;&c_l;&c_je;&c_t;)</trd>,
      <trd>&c_k;&c_a;&c_t;&c_a;&c_s;&c_t;&c_r;&c_o;&c_f;&c_i;&c_r;&c_a;&c_m;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>estavellar-se</trd>,
      <trd>caure a terra</trd>,
      <trd>desplomar-se</trd>
    </trdgrp>   
    <trdgrp lng="fr">
      <trd>se <ind>crasher</ind></trd>,
      <trd>s'<ind>&eacute;craser</ind><klr> (&agrave; terre)</klr></trd>
    </trdgrp>
    <trd lng="hu">lezuhan <klr tip="ind">(rep&uuml;l&odblac;g&eacute;p)</klr></trd>
    <trdgrp lng="it">
      <trd>schiantarsi</trd>,
      <trd>fracassarsi</trd>,
      <trd>sfracellarsi</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>espatifar-se</trd>,
      <trd>esborrachar-se</trd>
    </trdgrp>
    <trd lng="nl">neerstorten</trd>
    <trd lng="pl">rozbi&cacute; si&eogonek; <klr>(np. samolot)</klr></trd> 
    <trdgrp lng="ro">
      <trd>rupe</trd>,
      <trd>sparge</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_b;&c_i;&c_t;&c_mol;&c_s;&c_ja;<klr tip="amb">
      (&c_o; &c_s;&c_a;&c_m;&c_o;&c_l;&c_jo;&c_t;&c_je;)</klr></trd>,
      <trd>&c_p;&c_o;&c_t;&c_je;&c_r;&c_p;&c_je;&c_t;&c_mol;
      <ind>&c_k;&c_a;&c_t;&c_a;&c_s;&c_t;&c_r;&c_o;&c_f;&c_u;</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="krasx.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <klr>(pri borsaj kurzoj)</klr> Subite falegi, ka&ubreve;zante panikajn vendojn kaj
      gravajn perdojn al akciuloj:
      <ekz>
        akcioj de &ccirc;iaj tiuspecaj entreprenoj anta&ubreve; kelka tempo saltegis supren, sed
        &gcirc;uste nun ili falegas, <tld/>as suben, la unuaj bankrotoj kaj ruini&gcirc;oj okazis
        <fnt>
          <bib>Monato</bib>
          <aut>MAUL Stefan</aut>
          <vrk><url ref="&Steloj;monato2/007123.html">Defio a&ubreve; devio de politiko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">abst&uuml;rzen</trd>
  <trd lng="uk">&#x440;&#x43E;&#x437;&#x431;&#x438;&#x442;&#x438;&#x441;&#x44F; (&#x43F;&#x440;&#x438; &#x43F;&#x430;&#x434;&#x456;&#x43D;&#x43D;&#x456;)</trd>
</drv>

<drv mrk="krasx.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="krasx.0.SON">
    <dif>
      <ref tip="lst" cel="son.0o" lst="voko:sonoj">Sono</ref>
      de io fale frakasi&gcirc;anta:
      <ekz>
        &scirc;i kokete retiris sian manon, <tld/>! la taso
        <klr>[&#x2026;]</klr> plon&gcirc;is sur la plankon
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 80a, volumo 2a, p.
        671a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_b;&c_u;&c_m;</trd>,
    <trd>&c_t;&c_r;&c_ja;&c_s;</trd>
  </trdgrp>
  <trd lng="ca">patapam!</trd>
  <trdgrp lng="fr">
    <trd>badaboum</trd>,
    <trd>patatras</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>puff!</trd>,
    <trd>bumm!</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>badabum</trd>,
    <trd>patatrac</trd>,
    <trd>crash</trd>
  </trdgrp>
  <trd lng="nl">boem!</trd>
  <trdgrp lng="pl">
    <trd>buum</trd>,
    <trd>trrrach!</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_u;&c_m;!</trd>,
    <trd>&c_t;&c_r;&c_a;&c_h;!</trd>,
    <trd>&c_t;&c_r;&c_a;&c_h;-&c_t;&c_a;&c_r;&c_a;&c_r;&c_a;&c_h;!</trd>
  </trdgrp>
</drv>

<drv mrk="krasx.0o">
  <kap><tld/>o</kap>
  <snc mrk="krasx.0o.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      Falego kun frakasi&gcirc;o:
      <ekz>
        helikoptera <tld/>o pro rompo de rotora lameno
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2012/010437.php">Usonangla, &ccirc;u efika mondolingvo?</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvar nukleaj centraloj la&ubreve;dire estas nesufi&ccirc;e fortikaj por protekti kontra&ubreve;
        <tld/>o de eta aviadilo
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2011/010418.php">Adia&ubreve;o al nukleo</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        specialisto tutmonda pri aviadilaj <tld/>oj
        <fnt>
          <bib>MD02</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>o <klr>(t.e. <ref cel="kolaps.0o.FIG">kolapso<sncref/></ref>)</klr>
        de socialismo
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 7 (57)</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Absturz</trd>,
      <trd>Crashlandung</trd>
    </trdgrp>    
  </snc>
  <snc mrk="krasx.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Subita runiniga <ref tip="super" cel="fal.0o">fal</ref>ego de kurzoj:
      <ekz>
        misfaroj <klr>[&#x2026;]</klr>, kiuj ka&ubreve;zis la
        <ind>borso<tld/>o</ind>n en 1987
        <fnt>
          <aut>D. E. Rogers</aut>,
          <vrk>Ekonomia miraklo a&ubreve; malsano?</vrk>,
          <bib>Monato</bib>,
          <lok>1997:2, p. 14a</lok>
        </fnt>.
        <trd lng="ca">crac borsari</trd>
      </ekz>
    </dif>
    <trd lng="de">B&ouml;rsenkrach</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_t;&c_a;&c_s;&c_t;&c_r;&c_o;&c_f;&c_a;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_h;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_r;&c_a;&c_z;&c_b;&c_i;&c_v;&c_a;&c_n;&c_je;</trd>,
    <trd>&c_k;&c_a;&c_t;&c_a;&c_s;&c_t;&c_r;&c_o;&c_f;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">desplomament</trd>
  <trdgrp lng="fr">
    <trd>crash <klr>(falego)</klr></trd>
    <trd>krach <klr>(ruini&gcirc;o)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>krach</trd>,
    <trd>zuhan&aacute;s</trd>,
    <trd>puffan&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>schianto</trd>,
    <trd>fracassamento</trd>,
    <trd>sfracellamento</trd>,
    <trd>disastro<klr> aereo</klr></trd>,
    <trd>crack<klr> (econ.)</klr></trd>,
    <trd>patatrac</trd>
  </trdgrp>
  <trd lng="nl">crash</trd>
  <trd lng="pl">kraksa</trd>
  <trd lng="pt">queda ou desastre com avi&atilde;o</trd>
  <trd lng="ro">carambolaj</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_t;&c_a;&c_s;&c_t;&c_r;&c_o;&c_f;&c_a;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_h;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x430;&#x432;&#x430;&#x440;&#x456;&#x44F;</trd>,
    <trd>&#x43A;&#x430;&#x442;&#x430;&#x441;&#x442;&#x440;&#x43E;&#x444;&#x430;</trd>
  </trdgrp>
</drv>
</art>

