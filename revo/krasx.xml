<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: krasx.xml,v 1.25 2023/12/03 09:59:00 revo Exp $">
<kap>
  <rad>kra&scirc;</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="krasx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="krasx.0i.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      <ref tip="super" cel="fal.0i.mallevigxi">Fal</ref>egi kaj
      <ref cel="frakas.0igxi">frakasi&gcirc;i</ref>:
      <ekz>
        la flugma&scirc;ino komencis deteri&gcirc;i kaj <tld/>is en alian flugma&scirc;inon, kiu tre
        malfeli&ccirc;e hazarde ankora&ubreve; baris la vojon
        <fnt>
          <aut>Pejno Simono</aut>
            <vrk><url ref="&Monato;/2000/007143.php">&Ccirc;u ni mortu miskomprene? (2)</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 23-25</lok>
          </fnt>;
      </ekz>
      <ekz>
        la 16an de julio <tld/>is albana registara helikoptero en Adriatikon
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2006/009052.php">&Ccirc;u akcidento a&ubreve; murdo?</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        pasintjare <tld/>is en Litovio 50 km for de <nom>Kaunas</nom> rusa &ccirc;asaviadilo
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2006/008804.php">Post kra&scirc;o levi&gcirc;as esperoj</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        ma&scirc;ino C-130 de la irana aer-armeo paneis kaj <tld/>is apud 10-eta&gcirc;a lo&gcirc;ejo
        <fnt>
          <bib>Monato</bib>
          <aut>Keyhan Sayadpour</aut>
          <vrk><url ref="&Monato;/2006/008856.php">Embargo kontribuis al katastrofo</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_z;&c_mol;&c_b;&c_ib;&c_c;&c_c;&c_a; <klr tip="amb">(&c_p;&c_r;&c_a;
      &c_s;&c_a;&c_m;&c_a;&c_l;&c_jo;&c_t;)</klr></trd>,
      <trd>&c_p;&c_a;&c_c;&c_ja;&c_r;&c_p;&c_je;&c_c;&c_mol;
      <ind>&c_k;&c_a;&c_t;&c_a;&c_s;&c_t;&c_r;&c_o;&c_f;&c_u;</ind></trd>
    </trdgrp>
    <trdgrp lng="bg">
      <trd>&c_r;&c_a;&c_z;&c_b;&c_i;&c_v;&c_a;&c_m; &c_s;&c_je; (&c_z;&c_a; &c_s;&c_a;&c_m;&c_o;&c_l;&c_je;&c_t;)</trd>,
      <trd>&c_k;&c_a;&c_t;&c_a;&c_s;&c_t;&c_r;&c_o;&c_f;&c_i;&c_r;&c_a;&c_m;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>estavellar-se</trd>,
      <trd>caure a terra</trd>,
      <trd>desplomar-se</trd>
    </trdgrp>   
    <trdgrp lng="fr">
      <trd>se <ind>crasher</ind></trd>,
      <trd>s'<ind>&eacute;craser</ind><klr> (&agrave; terre)</klr></trd>
    </trdgrp>
    <trd lng="hu">lezuhan <klr tip="ind">(rep&uuml;l&odblac;g&eacute;p)</klr></trd>
    <trdgrp lng="it">
      <trd>schiantarsi</trd>,
      <trd>fracassarsi</trd>,
      <trd>sfracellarsi</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>espatifar-se</trd>,
      <trd>esborrachar-se</trd>
    </trdgrp>
    <trd lng="nl">neerstorten</trd>
    <trd lng="pl">rozbi&cacute; si&eogonek; <klr>(np. samolot)</klr></trd> 
    <trdgrp lng="ro">
      <trd>rupe</trd>,
      <trd>sparge</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_b;&c_i;&c_t;&c_mol;&c_s;&c_ja;<klr tip="amb">
      (&c_o; &c_s;&c_a;&c_m;&c_o;&c_l;&c_jo;&c_t;&c_je;)</klr></trd>,
      <trd>&c_p;&c_o;&c_t;&c_je;&c_r;&c_p;&c_je;&c_t;&c_mol;
      <ind>&c_k;&c_a;&c_t;&c_a;&c_s;&c_t;&c_r;&c_o;&c_f;&c_u;</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="krasx.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <klr>(pri borsaj kurzoj)</klr> Subite falegi, ka&ubreve;zante panikajn vendojn kaj
      gravajn perdojn al akciuloj:
      <ekz>
        akcioj de &ccirc;iaj tiuspecaj entreprenoj anta&ubreve; kelka tempo saltegis supren, sed
        &gcirc;uste nun ili falegas, <tld/>as suben, la unuaj bankrotoj kaj ruini&gcirc;oj okazis
        <fnt>
          <aut>Stefan Maul</aut>
            <vrk><url ref="&Monato;/2000/007123.html">Defio a&ubreve; devio de politiko</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 6</lok>
          </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_e;&c_z;&c_k;&c_a; <ind>&c_p;&c_a;&c_d;&c_a;&c_c;&c_mol;</ind> <klr>(&c_a;&c_b; &c_a;&c_k;&c_c;&c_y;&c_ja;&c_h; &c_ib; &c_d;&c_a; &c_g;. &c_p;.)</klr></trd>
  </snc>
  <trd lng="de">abst&uuml;rzen</trd>
  <trdgrp lng="ja">
    <trd>&#x589C;&#x843D;&#x3059;&#x308B;  <pr>&#x3064;&#x3044;&#x3089;&#x304F;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x6FC0;&#x7A81;&#x3059;&#x308B;  <pr>&#x3052;&#x304D;&#x3068;&#x3064;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="uk">&c_r;&c_o;&c_z;&c_b;&c_i;&c_t;&c_i;&c_s;&c_ja; (&c_p;&c_r;&c_i; &c_p;&c_a;&c_d;&c_ib;&c_n;&c_n;&c_ib;)</trd>
  <trdgrp lng="zh">
    <trd>&#x5760;&#x5165; <pr>zhu&#xEC;r&#xF9;</pr></trd>,
    <trd>&#x5760;&#x6BC1; <pr>zhu&#xEC;hu&#x1D0;</pr></trd>,
    <trd>&#x6454; <pr>shu&#x101;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="krasx.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="krasx.0.SON">
    <dif>
      <ref tip="lst" cel="son.0o" lst="voko:sonoj">Sono</ref>
      de io fale frakasi&gcirc;anta:
      <ekz>
        &scirc;i kokete retiris sian manon, <tld/>! la taso
        <klr>[&#x2026;]</klr> plon&gcirc;is sur la plankon
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 80a, volumo 2a, p.
        671a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_b;&c_u;&c_m;</trd>,
    <trd>&c_t;&c_r;&c_ja;&c_s;</trd>
  </trdgrp>
  <trd lng="ca">patapam!</trd>
  <trdgrp lng="fr">
    <trd>badaboum</trd>,
    <trd>patatras</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>puff!</trd>,
    <trd>bumm!</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>badabum</trd>,
    <trd>patatrac</trd>,
    <trd>crash</trd>
  </trdgrp>
  <trd lng="nl">boem!</trd>
  <trdgrp lng="pl">
    <trd>buum</trd>,
    <trd>trrrach!</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_u;&c_m;!</trd>,
    <trd>&c_t;&c_r;&c_a;&c_h;!</trd>,
    <trd>&c_t;&c_r;&c_a;&c_h;-&c_t;&c_a;&c_r;&c_a;&c_r;&c_a;&c_h;!</trd>
  </trdgrp>
  <trd lng="zh">&#x6B3B; <pr>chu&#x101;</pr></trd>
</drv>

<drv mrk="krasx.0o">
  <kap><tld/>o</kap>
  <snc mrk="krasx.0o.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      Falego kun frakasi&gcirc;o:
      <ekz>
        helikoptera <tld/>o pro rompo de rotora lameno
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2012/010437.php">Usonangla, &ccirc;u efika mondolingvo?</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvar nukleaj centraloj la&ubreve;dire estas nesufi&ccirc;e fortikaj por protekti kontra&ubreve;
        <tld/>o de eta aviadilo
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2011/010418.php">Adia&ubreve;o al nukleo</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        specialisto tutmonda pri aviadilaj <tld/>oj
        <fnt>
          <bib>MD02</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>o <klr>(t.e. <ref cel="kolaps.0o.FIG">kolapso<sncref/></ref>)</klr>
        de socialismo
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 7 (57)</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        popolistoj en Rusio &scirc;atas malbeni la nomojn de la tiutempaj &leftquot;junaj reformistoj&rightquot;,
        kaj kulpigi ilin, kune kun Mi&hcirc;ail Gorba&ccirc;ov, pri la <tld/>o de la imperio
        <fnt>
          <bib>Monato</bib>
          <aut>Kalle Kniivil&auml;</aut>
          <vrk><url ref="&Monato;/2007/009159.php">15 jarojn post la pereo de la imperio</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Absturz</trd>,
      <trd>Crashlandung</trd>
    </trdgrp>    
    <trdgrp lng="zh">
    <trd>&#x5760;&#x6BC1; <pr>zhu&#xEC;hu&#x1D0;</pr></trd>,
    <trd>&#x6454;&#x707D; <pr>shu&#x101;iz&#x101;i</pr></trd>,
    <trd>&#x5760;&#x843D; <pr>zhu&#xEC;lu&#xF2;</pr></trd>,
    <trd>&#x5F02;&#x5E38;&#x7ED3;&#x675F; <pr>y&#xEC;ch&#xE1;ngji&#xE9;sh&#xF9;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="krasx.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Subita runiniga <ref tip="super" cel="fal.0o">fal</ref>ego de kurzoj:
      <ekz>
        misfaroj <klr>[&#x2026;]</klr>, kiuj ka&ubreve;zis la
        <ind>borso<tld/>o</ind>n en 1987
        <fnt>
          <aut>D. E. Rogers</aut>,
          <vrk>Ekonomia miraklo a&ubreve; malsano?</vrk>,
          <bib>Monato</bib>,
          <lok>1997:2, p. 14a</lok>
        </fnt>.
        <trd lng="ca">crac borsari</trd>
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>B&ouml;rsenkrach</trd>,
      <trd>Kurssturz</trd>
    </trdgrp>    
  </snc>
  <snc mrk="krasx.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Misfunkcia halto de komputila operaciumo.
    </dif>
    <trdgrp lng="de">
      <trd>Rechnerabsturz</trd>,
      <trd>Computerabsturz</trd>,
      <trd>Absturz</trd>
    </trdgrp>    
  </snc>
  <refgrp tip="vid">
    <ref cel="fal.dis0o.kadukigxo">disfalo<sncref/></ref>,
    <ref cel="kolaps.0i.FIG">kolapso<sncref/></ref>,
    <ref cel="kriz.0o.KOMUNE">krizo<sncref/></ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_t;&c_a;&c_s;&c_t;&c_r;&c_o;&c_f;&c_a;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_h;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_r;&c_a;&c_z;&c_b;&c_i;&c_v;&c_a;&c_n;&c_je;</trd>,
    <trd>&c_k;&c_a;&c_t;&c_a;&c_s;&c_t;&c_r;&c_o;&c_f;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">desplomament</trd>
  <trdgrp lng="fr">
    <trd>crash <klr>(falego)</klr></trd>
    <trd>krach <klr>(ruini&gcirc;o)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>krach</trd>,
    <trd>zuhan&aacute;s</trd>,
    <trd>puffan&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>schianto</trd>,
    <trd>fracassamento</trd>,
    <trd>sfracellamento</trd>,
    <trd>disastro<klr> aereo</klr></trd>,
    <trd>crack<klr> (econ.)</klr></trd>,
    <trd>patatrac</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x589C;&#x843D;  <pr>&#x3064;&#x3044;&#x3089;&#x304F;</pr></trd>,
    <trd>&#x6FC0;&#x7A81;  <pr>&#x3052;&#x304D;&#x3068;&#x3064;</pr></trd>,
    <trd>&#x885D;&#x7A81;  <pr>&#x3057;&#x3087;&#x3046;&#x3068;&#x3064;</pr></trd>,
    <trd>&#x7834;&#x58CA;  <pr>&#x306F;&#x304B;&#x3044;</pr></trd>,
    <trd>&#x682A;&#x306E;&#x66B4;&#x843D;  <pr>&#x304B;&#x3076;&#x306E;&#x307C;&#x3046;&#x3089;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">crash</trd>
  <trd lng="pl">kraksa</trd>
  <trd lng="pt">queda ou desastre com avi&atilde;o</trd>
  <trd lng="ro">carambolaj</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_t;&c_a;&c_s;&c_t;&c_r;&c_o;&c_f;&c_a;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_h;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&c_v;&c_a;&c_r;&c_ib;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_t;&c_a;&c_s;&c_t;&c_r;&c_o;&c_f;&c_a;</trd>
  </trdgrp>
</drv>
</art>

