<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: georgi.xml,v 1.23 2025/08/12 22:32:15 revo Exp $">
<kap>
  <rad>georgin</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="georgi.0o">
  <kap><tld/>o</kap>
  <snc mrk="georgi.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="dif" cel="dali.0o">Dalio</ref>:
      <ekz>
        kelkaj <tld/>oj kun pezaj kapoj, kiuj elrigardis super la palisaro de la bone
        tenata &gcirc;ardeneto
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i prenis el vazo, karese etendinte al mi,
        kaj mi ma&scirc;inece kunpremis &gcirc;in &mdash; <tld/>on
        de &ccirc;eriza koloro
          <fnt>
            <aut>A. Grin, trad. J. Finkel</aut>,
            <vrk><url ref="http://www.esperanto.mv.ru/Grin/ora.html">
            La ora &ccirc;eno</url></vrk>,
            <lok>2009</lok>
          </fnt>.
      </ekz>
    </dif>
    <bld
    lok="&WCU;/thumb/a/af/Sima_&Zcaron;iki&cacute;%2C_Georgine%2C_1961..JPG/640px-Sima_&Zcaron;iki&cacute;%2C_Georgine%2C_1961..JPG"
    prm="CC BY-SA 3.0">Sima &Zcaron;iki&cacute;, <tld/>o, 1961
        <fnt>
          <aut>Zavi&ccaron;ajni muzej Knja&zcaron;evac</aut>
          <lok><url
          ref="&WCW;/File:Sima_%C5%BDiki%C4%87,_Georgine,_1961..JPG">&WCW;/File:Sima_&Zcaron;iki&cacute;,_Georgine,_1961..JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_r;&c_g;&c_ib;&c_n;&c_ja;</trd>
  <trdgrp lng="bg">
    <trd>&c_d;&c_a;&c_l;&c_i;&c_ja;</trd>,
    <trd>&c_g;&c_je;&c_r;&c_g;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">d&agrave;lia</trd>
  <trdgrp lng="cs">
    <trd>georg&iacute;na</trd>,
    <trd>ji&rcaron;ina</trd>,
    <trd>ji&rcaron;inka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Dahlie</trd>,
    <trd>Georgine</trd>
  </trdgrp>
  <trd lng="es">dalia</trd>
  <trd lng="fr">dahlia</trd>
  <trd lng="hu">georgina</trd>
  <trd lng="nl">dahlia</trd>
  <trdgrp lng="pl">
    <trd>georginia</trd>,
    <trd>dalia</trd>
  </trdgrp>
  <trd lng="pt">d&aacute;lia</trd>
  <trd lng="ru">&c_g;&c_je;&c_o;&c_r;&c_g;&c_i;&c_n;</trd>
  <trd lng="sk">georg&iacute;na</trd>
  <trd lng="uk">&c_zh;&#x43E;&c_r;&c_zh;&c_i;&#x43D;&c_a;</trd>
  <trd lng="zh">&#22825;&#x7AFA;&#29281;&#x4E39; <pr>ti&amacron;nzh&#xFA;m&#x1D4;d&amacron;n</pr></trd>
</drv>
</art>
