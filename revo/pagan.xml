<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>pagan</rad>/o</kap>
<drv mrk="pagan.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Adepto de <tld/>ismo.
    </dif>
  </snc>
  <trd lng="de">Heide</trd>
  <trd lng="nl">heiden</trd>
  <trd lng="ru">&c_ja;&c_z;&c_y;&c_ch;&c_n;&c_i;&c_k;</trd>
</drv>

<drv mrk="pagan.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      Greka a&ubreve; romana politeismo.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Paganismus</trd>,
    <trd>Heidentum</trd>
  </trdgrp>
  <trd lng="nl">heidendom</trd>
  <trd lng="ru">&c_ja;&c_z;&c_y;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>
</art>
