<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>&hcirc;ord</rad>/o
</kap>
<drv mrk="hxord.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      &Ccirc;e la <ref tip="malprt" cel="hxord.0uloj"><tld/>uloj</ref>,
      elasta &scirc;nuro inter spina medolo kaj intesta tubo. &Ccirc;e
      la vertebruloj &gcirc;i ekzistas nur &ccirc;e la embrio, sed poste
      anstata&ubreve;igata de la vertebraro.
    </dif>
    <trd lng="la">chorda dorsalis</trd>
  </snc>
</drv>
<drv mrk="hxord.0uloj">
  <kap><tld/>uloj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Filumo de la <ref tip="super" cel="best.0oj">bestoj</ref>
      (<trd lng="la">Chordata</trd>), havantaj <tld/>on.
    </dif>
  </snc>
  <trd lng="de">Chordatiere</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>