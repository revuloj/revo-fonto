<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>balt</rad>/o
    </kap>

    <drv mrk="balt.0o">
      <kap><tld/>o</kap>
      <snc mrk="balt.0o.praa">
	<uzo tip="fak">HIS</uzo>
	<dif>
	  Ano de e&ubreve;ropa superetno, kiu en la 1a jarmilo p.K. prilo&gcirc;is 
	  la teritorion de la sudorienta bordo de la <tld lit="B"/>a Maro
	  &gcirc;is la norda Dnepro kaj la baseno de
	  <ref cel="oka.O0o">Okao</ref>, kaj kies modernaj
	  reprezentantoj estas
	  <refgrp tip="sub">
	    <ref cel="latv.0o">latvoj</ref> kaj
	    <ref cel="litov.0o">litovoj</ref>
	  </refgrp>:
	  <ekz>
	    okcidentaj <tld/>oj estis
	    <ref tip="sub" cel="prus.0o.balto">prusoj</ref>
	    <fnt>
	      <aut>P. &Ccaron;eliauskas</aut>:
	      <vrk>Litovio dum jarcentoj</vrk>.
	      <lok>Kaunas, 2002. P. 13.</lok>
	    </fnt>.
	  </ekz>
	</dif>
      </snc>
      <trd lng="en">Balt</trd>
      <trd lng="es">balto</trd>
      <trd lng="ru">&c_b;&c_a;&c_l;&c_t;</trd>
    </drv>

    <drv mrk="balt.0a">
      <kap><tld/>a</kap>

      <snc mrk="balt.0a.etne">
	<uzo tip="fak">HIS</uzo>
	<uzo tip="fak">LIN</uzo>
	<dif>
	  Rilata al <tld/>oj, ilia lingvo ktp:
	  <ekz>
	    hodia&ubreve; el <klr>la</klr> <ind><tld/>aj lingvoj</ind>
	    vivas nur du: la litova kaj la latva
	    <fnt>
	      <aut>P. &Ccaron;eliauskas</aut>:
	      <vrk>Litovio dum jarcentoj</vrk>.
	      <lok>Kaunas, 2002. P. 7.</lok>
	    </fnt>.
	    <trd lng="ru">&c_b;&c_a;&c_l;&c_t;&c_i;&c_j;&c_s;&c_k;&c_i;&c_je; &c_ja;&c_z;&c_y;&c_k;&c_i;</trd>
	  </ekz>
	</dif>
      </snc>
      <snc mrk="balt.0a.Baltia">
	<dif>
	  Baltia, rilata al la <ref><tld lit="B"/>a Maro</ref>:
	  <ekz>
	    la <tld/>aj &scirc;tatoj.
	  </ekz>
	</dif>
      </snc>
      <trd lng="ru">&c_b;&c_a;&c_l;&c_t;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j;</trd>
    </drv>

    <drv mrk="balt.B0aMaro">
      <kap><tld lit="B"/>a Maro</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
	<dif>
	  <ref tip="malprt" cel="atlant.0o">Atlantika</ref> maro
	  norde en E&ubreve;ropo, inter Pollando kaj Svedujo:
	  <ekz>
	    apud la Balta Maro &ccirc;e la Danaj golfoj...
	    <fnt><bib>Fab1</bib>, <lok>Fluganta kofro</lok></fnt>.
	  </ekz>
	</dif>
      </snc>
      <trd lng="en">Baltic Sea</trd>
      <trd lng="en">Mar B&aacute;ltico</trd>
      <trd lng="ru">&c_B;&c_a;&c_l;&c_t;&c_i;&c_j;&c_s;&c_k;&c_o;&c_je; &c_m;&c_o;&c_r;&c_je;</trd>
    </drv>

    <drv mrk="balt.0aslava">
      <kap><tld/>a-slava</kap>
      <snc>
	<uzo tip="fak">LIN</uzo>
	<dif>
	  Rilata tiu minimuma grupo da
	  <ref tip="super" cel="hind.0euxropa">hinde&ubreve;ropaj</ref>
	  lingvoj, kiu entenas la lingvojn <tld/>ajn kaj
	  <ref tip="sub" cel="slav.0a.lingvo">slavajn</ref>.
	</dif>
      </snc>
      <trdgrp lng="en">
	<trd>Balto-Slavic</trd>,
	<trd>Balto-Slavonic</trd>
      </trdgrp>
      <trd lng="en">baltoeslavo</trd>
      <trd lng="ru">&c_b;&c_a;&c_l;&c_t;&c_o;-&c_s;&c_l;&c_a;&c_v;&c_ja;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
    </drv>

  </art>

