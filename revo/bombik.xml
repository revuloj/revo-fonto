<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bombik.xml,v 1.24 2023/08/31 08:48:52 revo Exp $">
<kap>
<rad>bombiks</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="bombik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="bombik1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Bombyx</trd>), el kies kokono oni fabrikas la
      <ref tip="vid" cel="silk.0o">silkon</ref>:
      <ekz>
        kiam <tld/>o estas ra&ubreve;po, &gcirc;i produktas altvaloran fibron, nutritante 
        sin per folioj de blanka moruso, &gcirc;i elkorpigas unikan fadenon el 
        kruda silko kaj per &ccirc;i-lasta konstruas sian kokonon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=48836">Silkra&ubreve;po</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="rauxp.silk0o.ZOO">silkra&ubreve;po</ref>
  <trdgrp lng="be">
    <trd>&c_sh;&c_a;&c_w;&c_k;&c_a;&c_p;&c_r;&c_a;&c_d;</trd>,
    <trd>&c_ja;&c_d;&c_v;&c_a;&c_b;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>b&ograve;mbix</trd>,
    <trd>cuc de seda</trd>
  </trdgrp>
  <trd lng="de">Seidenspinner</trd>
  <trdgrp lng="en">
    <trd>bombyx</trd>,
    <trd>silkworm</trd>
  </trdgrp>
  <trd lng="fr">bombyx</trd>
  <trd lng="he">&tav;&vav;&lamed;&ayin;&tav; &mem;&shin;&yod;</trd>
  <trd lng="hu">sz&ouml;v&odblac;lepke</trd>
  <trd lng="pl">jedwabnik</trd>
  <trd lng="ru">&c_sh;&c_je;&c_l;&c_k;&c_o;&c_p;&c_r;&c_ja;&c_d;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>bourec</trd>,
    <trd>hedv&aacute;bn&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30AB;&#x30A4;&#x30B3;&#x30AC;</trd>,
    <trd>&#x30AB;&#x30A4;&#x30B3;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hodv&aacute;bnik</trd>,
    <trd>priadka moru&scaron;ov&aacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x448;&#x43E;&#x432;&#x43A;&#x43E;&#x43F;&#x440;&#x44F;&#x434;</trd>,
    <trd>&#x448;&#x43E;&#x432;&#x43A;&#x43E;&#x432;&#x438;&#x43A;</trd>
  </trdgrp>
</drv>

</art>
