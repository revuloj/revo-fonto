<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>bombiks</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="bombik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="bombik.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Bombyx</trd>), el kies kokono oni fabrikas la silkon.
    </dif>
  </snc>
  <trd lng="de">Seidenspinner</trd>
  <trd lng="ru">&c_sh;&c_je;&c_l;&c_k;&c_o;&c_p;&c_r;&c_ja;&c_d;</trd>
</drv>
<drv mrk="bombik.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="lepido1.0oj">lepidopteroj</ref>
      (<trd lng="la">Bombycidae</trd>).
    </dif>
  </snc>
</drv>
</art>
