<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>bombiks</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="bombik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="bombik1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Bombyx</trd>), el kies kokono oni fabrikas la
<ref tip="vid" cel="silk.0o">silkon</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_a;&c_w;&c_k;&c_a;&c_p;&c_r;&c_a;&c_d;</trd>,
    <trd>&c_ja;&c_d;&c_v;&c_a;&c_b;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Seidenspinner</trd>
  <trd lng="fr">bombyx</trd>
  <trd lng="he">&tav;&vav;&lamed;&ayin;&tav; &mem;&shin;&yod;</trd>
  <trd lng="hu">sz&ouml;v&odblac;lepke</trd>
  <trd lng="ru">&c_sh;&c_je;&c_l;&c_k;&c_o;&c_p;&c_r;&c_ja;&c_d;</trd>
</drv>

</art>
