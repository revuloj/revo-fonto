<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>udon</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="udon.0o">
  <kap><tld/>o</kap>
  <snc mrk="udon.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Japana <ref tip="super" cel="nudel.0o.cxia">nudelo</ref> el tritika
      faruno, plejofte dika, iom elasta:
      <ekz>
        <tld/>o en supo
        <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>&leftquot;iun tagon por mortigi tiun
        memoron&rightquot;, p. 233a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">udon</trd>
  <trd lng="pt">udon</trd>
</drv>
</art>

