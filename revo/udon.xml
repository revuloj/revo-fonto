<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: udon.xml,v 1.12 2024/04/10 20:23:44 revo Exp $">
<kap>
    <rad>udon</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="udon.0o">
  <kap><tld/>o</kap>
  <snc mrk="udon.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Japana <ref tip="super" cel="nudel.0o.cxia">nudelo</ref> el
      tritika faruno, plej ofte dika, iom elasta:
      <ekz>
        <tld/>o en supo
        <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>&leftquot;iun tagon
        por mortigi tiun memoron&rightquot;, p. 233a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>o stufita kun miso<klr>[o]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=26300">Nagojo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>fideu gros</trd>,
    <trd>udon</trd>
  </trdgrp>
  <trd lng="en">udon</trd>
  <trd lng="de">Udon</trd>
  <trd lng="fi">udon <klr> (-nuudeli)</klr></trd>
  <trd lng="fr">udon</trd>
  <trd lng="it">udon</trd>
  <trd lng="ja">&#12358;&#12393;&#12435;</trd>
  <trd lng="pl">udon <klr> (rodzaj makaronu jap.)</klr></trd>
  <trd lng="pt">udon</trd>
  <trd lng="sv">udon</trd>
  <trd lng="uk">&c_u;&c_d;&#x43E;&#x43D; (&#x43C;&c_a;&#x43A;&c_a;&c_r;&#x43E;&#x43D;&c_i;)</trd>
</drv>
</art>

