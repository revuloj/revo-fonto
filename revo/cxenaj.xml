<drv mrk="cxenaj.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="cxenaj.0o.urbo">
    <dif>
      Granda haven<ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      en la sudoriento de <ref tip="malprt" cel="barat.B0o">Barato</ref>:
      <ekz>
        de <tld/>o mi iris al Kolkato kie mi trovis min en malfacila&jcirc;oj
        <fnt>
          <vrk><url ref="http://agrao.50webs.com/gandhi/eksperimentoj/dua/29_revenu_baldau.htm">La
          A&ubreve;tobiografio de Gandhi</url></vrk>
          <lok> Parto 2.29 &leftquot;Revenu balda&ubreve;&rightquot;</lok>
        </fnt>;
      </ekz>
      <ekz>
        la nomo <tld/>o strange devenas de la telugua lingvo (dum la &scirc;tata kaj urba
        lingvo estas la tamila) kaj signifas bela urbo
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/%C4%88enajo">&Ccirc;enajo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        altlernejoj kaj ekspoziciejoj en diversaj partoj de <tld/>o servu kiel
        provizoraj gastejoj
        <fnt>
          <vrk><url ref="https://communalnews.com/eo/coronavirus-tamil-nadu-registras-pli-ol-600-novajn-kazojn/">Coronavirus - Tamil Nadu Records Pli ol 600 Novaj Kazoj ...</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_Ch;&c_e;&c_n;&c_a;&c_ib;</trd>
    <trd lng="de">Chennai</trd>
    <trd lng="en">Chennai</trd>
    <trd lng="fr">Chennai</trd>
    <trdgrp lng="ta">
      <trd>&#xB9A;&#xBC6;&#xBA9;&#xBCD;&#xBA9;&#xBC8;</trd>,
      <trd>&#xBAE;&#xBC6;&#xB9F;&#xBCD;&#xBB0;&#xBBE;&#xBB8;&#xBCD;</trd>
    </trdgrp>
    <trd lng="zh">&#x91D1;&#x5948; <pr>j&#x12B;nn&#xE0;i</pr></trd>
</snc>
