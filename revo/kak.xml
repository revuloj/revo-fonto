<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kak.xml,v 1.2 2025/12/23 08:43:36 afido Exp $">
<kap>
    <rad>kak</rad>/i <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="kak.0i">
  <kap><tld/>i</kap>
  <snc mrk="kak.0i.FIZL">
    <uzo tip="klr">(familiare)</uzo>
    <dif>
      <ref tip="dif" cel="fek.0i">Feki</ref>:
      <ekz>
        <klr>[li]</klr> terure eksakras, kiam vidas, ke la bebo intertempe <tld/>is en
        lian liton
        <fnt>
          <bib>KrB</bib>
          <lok>Ne kotas besto en sia nesto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;an&gcirc;i plen<tld/>itan vindotukon;
      </ekz>
      <ekz>
        mi ne &scirc;atas <tld/>i en kvar muroj, mi &scirc;atas fari tion malka&scirc;e
        &ccirc;ar mi &leftquot;deziras sperti novan nivelon de libereco,
        vivante en plena harmonio kun la naturo&rightquot;
        <fnt>
          <aut>baraban</aut>
          <vrk>en: <url
          ref="https://uea.facila.org/artikoloj/movado/naturismo-kaj-esperanto-havas-komunajn-valorojn-r374">
          Naturismo kaj Esperanto havas komunajn valorojn</url></vrk>
          <lok>uea.facila, 2023-07-18</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>kacken</trd>,
    <trd>kackern</trd>,
    <trd>ein <ind>H&auml;ufchen</ind> machen</trd>
  </trdgrp>
  <trd lng="en">go <ind>poo-poo</ind></trd>
  <trdgrp lng="es">
    <trd>hacer(se) <ind>caca</ind></trd>,
    <trd>hacer &leftquot;<ind>pop&oacute;</ind>&rightquot;</trd>,
    <trd>hacer &leftquot;<ind>pos</ind>&rightquot;</trd>
  </trdgrp>
  <trdgrp lng="fi">
    <trd>kakata</trd>,
    <trd>kakkia</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>chier</trd>,
    <trd>faire <ind>caca</ind></trd>,
    <trd>caguer <klr>(en suda Fr.)</klr></trd>
  </trdgrp>
  <trd lng="hu">kak&aacute;l</trd>
  <trdgrp lng="it">
    <trd>fare la <ind>cacca</ind></trd>,
    <trd>cacare</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>drukken</trd>,
    <trd>een <ind>hoop</ind> doen</trd>,
    <trd>kakken</trd>,
    <trd>poepen</trd>
  </trdgrp>
  <trd lng="pt">fazer <ind>coc&oacute;</ind></trd>
  <trd lng="ru">&c_n;&c_a;&c_k;&c_a;&c_k;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">bajsa</trd>
</drv>

<drv mrk="kak.0i_sur">
  <kap>
    <tld/>i sur
  </kap>
  <snc>
    <uzo tip="stl">FRAZ</uzo>
    <dif>
      <klr>(mal&scirc;ate)</klr> Esti <ref cel="indife.0a">indiferenta</ref> al:
      <ekz>
        post sen&ccirc;esa petado, petegado, li montris sin du minutojn, kaj poste tute ne plu
        <klr>[&#x2026;]</klr>, nun mi <tld/>os sur lin
        <klr>(li estas nun por mi indiferenta)</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Petro Desmet'</aut>
          <vrk><url ref="&Monato;/2008/009429.php"
              >Surrealismo, surrealismo</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        la esprimo &#x201A;<tld/>i sur ion&#x2018;
        estas ja tre konata,
        kaj &ccirc;iam kun la signifo: ne zorgi pri &gcirc;i,
        ne interesi&gcirc;i pri &gcirc;i
        <fnt>
          <aut>P. Desmet'</aut>
          <vrk>Proverboj sub lupeo</vrk>
          <lok>2025</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="fajf.pri0i">fajfi pri</ref>
  </snc>
  <trdgrp lng="de">
    <trd>schei&szlig;en auf <klr>(etwas)</klr></trd>,
    <trd><klr>(jemand)</klr> kann mich mal <ind>kreuzweise</ind></trd>
  </trdgrp>
  <trd lng="fr">je lui dis <ind>merde&#x202F;</ind></trd>
</drv>

</art>

