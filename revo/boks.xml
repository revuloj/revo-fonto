<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
  <ofc>1</ofc>
  <rad>boks</rad>/i
  </kap>
  <drv mrk="boks.0i">
    <kap><tld/>i</kap>
    <gra><vspec>ntr</vspec></gra>
    <uzo tip="fak">SPO</uzo>
    <snc mrk="boks.0i.lukti">
      <dif>
      Pugnolukti. 
      </dif>
      <trd lng="de">boxen</trd>
      <trd lng="en">box</trd>
      <trd lng="es">boxear</trd>
      <trd lng="fr">boxer</trd>
      <trd lng="hu">bokszol</trd>
      <trd lng="pl">boksowa&cacute;</trd>
      <trd
lng="ru">&c_b;&c_o;&c_k;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
      <trd lng="sk">boxova&tcaron;</trd>
      <trd lng="sv">boxas</trd>
    </snc>

    <snc mrk="boks.0i.okupigxi">
    <dif>
      Okupi&gcirc;i pri <tld/>o kiel sportisto.
    </dif>
      <trd lng="pl">uprawia&cacute; <ind>boks</ind></trd>
    </snc>
  </drv>

  <drv mrk="boks.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">SPO</uzo>
    <snc>
      <dif>
        Arto, sporto de pugnolukto.
      </dif>
      <trd lng="be">&c_b;&c_o;&c_k;&c_s;</trd>
      <trdgrp lng="de">
         <trd>Boxen</trd>,
         <trd>Faustkampf</trd></trdgrp>
      <trdgrp lng="en">
        <trd>boxing</trd>,
        <trd>pugilism</trd>
      </trdgrp>
      <trdgrp lng="es">
        <trd>boxeo</trd>,
        <trd>box</trd>,
        <trd>pugilismo</trd>
      </trdgrp>
      <trd lng="fr">boxe <klr tip="ind">(sport)</klr></trd>
      <trdgrp lng="hu">
         <trd>&ouml;k&ouml;lv&iacute;v&aacute;s</trd>,
         <trd>boksz</trd></trdgrp>
      <trd lng="pl">boks</trd>
      <trd lng="ru">&c_b;&c_o;&c_k;&c_s;</trd>
      <trd lng="sk">box</trd>
      <trd lng="sv">boxning</trd>
    </snc>
  </drv>
</art>
