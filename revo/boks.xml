<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>boks</rad>/i
</kap>
<drv mrk="boks.0i">
<kap><tld/>i</kap>
<gra><vspec>ntr</vspec></gra>
<uzo tip="fak">SPO</uzo>
<snc>
<dif>
Pugnolukti. 
</dif>
</snc>
<trd lng="fr">boxer</trd>
<trd lng="ru">&c_b;&c_o;&c_k;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="boks.0o">
<kap><tld/>o</kap>
<uzo tip="fak">SPO</uzo>
<snc>
<dif>
Arto, sporto de pugnolukto.  
</dif>
</snc>
<trd lng="fr">boxe <klr tip="ind">(sport)</klr></trd>
<trd lng="ru">&c_b;&c_o;&c_k;&c_s;</trd>
</drv>
</art>
