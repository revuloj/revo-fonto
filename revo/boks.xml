<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
  <ofc>1</ofc>
  <rad>boks</rad>/i
  </kap>
  <drv mrk="boks.0i">
    <kap><tld/>i</kap>
    <gra><vspec>ntr</vspec></gra>
    <uzo tip="fak">SPO</uzo>
    <snc mrk="boks.0i.lukti">
      <dif>
      Pugnolukti:
      <ekz>
        personoj kiuj kvaza&ubreve; <tld/>as unu la alian kaj konkuras por
        akiri la regpovon
        <fnt>
          <aut>Platono, trad. Donald Broadribb</aut>
          <vrk>La Respubliko</vrk>
          <lok>parto 1</lok>
        </fnt>
      </ekz> 
      </dif>
      <trd lng="de">boxen</trd>
      <trd lng="en">box</trd>
      <trd lng="es">boxear</trd>
      <trd lng="hu">bokszol</trd>
      <trdgrp lng="it">
         <trd>boxare</trd>,
         <trd>pugilare</trd></trdgrp>
      <trd lng="pl">boksowa&cacute;</trd>
      <trd lng="ru">
&c_b;&c_o;&c_k;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
      <trd lng="sk">boxova&tcaron;</trd>
      <trd lng="sv">boxas</trd>
    </snc>

    <snc mrk="boks.0i.okupigxi">
    <dif>
      Okupi&gcirc;i pri <tld/>o kiel sportisto.
    </dif>
      <trdgrp lng="it">
         <trd>tirare di <ind>boxe</ind></trd>,
         <trd>praticare <ind>pugilato</ind></trd></trdgrp>
      <trd lng="pl">uprawia&cacute; <ind>boks</ind></trd>
    </snc>
  <trd lng="ca">boxar</trd>
  <trd lng="cs">boxovat</trd>
  <trd lng="fr">boxer</trd>
</drv>

  <drv mrk="boks.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">SPO</uzo>
    <snc>
      <dif>
        Arto, sporto de pugnolukto:
      <ekz>
        Al racie pensanta homo &scirc;ajnas strange, ke oni nomas <tld/>on
        <ctl>la nobla arto</ctl>
        <fnt>
          <aut>Jos&eacute; Dias Pinto</aut>
          <vrk>Brazila Esperantisto, 2014-07, numero 349a</vrk>
          <lok>La arto politiki</lok>
        </fnt>
      </ekz>
      </dif>
      <trd lng="be">&c_b;&c_o;&c_k;&c_s;</trd>
      <trd lng="ca">boxa</trd>
      <trdgrp lng="de">
         <trd>Boxen</trd>,
         <trd>Faustkampf</trd></trdgrp>
      <trdgrp lng="en">
        <trd>boxing</trd>,
        <trd>pugilism</trd>
      </trdgrp>
      <trdgrp lng="es">
        <trd>boxeo</trd>,
        <trd>box</trd>,
        <trd>pugilismo</trd>
      </trdgrp>
      <trd lng="fr">boxe <klr tip="ind">(sport)</klr></trd>
      <trdgrp lng="hu">
         <trd>&ouml;k&ouml;lv&iacute;v&aacute;s</trd>,
         <trd>boksz</trd></trdgrp>
      <trdgrp lng="it">
         <trd>boxe</trd>,
         <trd>pugilato</trd></trdgrp>
      <trd lng="pl">boks</trd>
      <trd lng="ru">&c_b;&c_o;&c_k;&c_s;</trd>
      <trd lng="sk">box</trd>
      <trd lng="sv">boxning</trd>
    </snc>
  <trd lng="cs">box</trd>
  <trd lng="sk">box <klr>(oddelen&eacute; miesto v re&scaron;taur&aacute;ci&iacute;)</klr></trd>
</drv>
</art>
