<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>boks</rad>/i
</kap>
<drv mrk="boks.0i">
<kap><tld/>i</kap>
<gra><vspec>ntr</vspec></gra>
<uzo tip="fak">SPO</uzo>
<snc>
<dif>
Pugnolukti. 
</dif>
</snc>
  <trd lng="de">boxen</trd>
  <trd lng="en">box</trd>
  <trd lng="es">boxear</trd>
  <trd lng="fr">boxer</trd>
  <trd lng="ru">&c_b;&c_o;&c_k;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">boxas</trd>
</drv>
<drv mrk="boks.0o">
<kap><tld/>o</kap>
<uzo tip="fak">SPO</uzo>
<snc>
<dif>
Arto, sporto de pugnolukto.  
</dif>
</snc>
<trdgrp lng="de">
   <trd>Boxen</trd>,
   <trd>Faustkampf</trd></trdgrp>
<trdgrp lng="en">
  <trd>boxing</trd>,
  <trd>pugilism</trd>
</trdgrp>
<trdgrp lng="es">
  <trd>boxeo</trd>,
  <trd>box</trd>,
  <trd>pugilismo</trd>
</trdgrp>
<trd lng="fr">boxe <klr tip="ind">(sport)</klr></trd>
<trd lng="ru">&c_b;&c_o;&c_k;&c_s;</trd>
<trd lng="sv">boxning</trd>
</drv>
</art>
