<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
    <art mrk="$Id: ventr.xml,v 1.28 2023/12/28 13:31:43 revo Exp $">
    <kap>
        <ofc>*</ofc>
        <rad>ventr</rad>/o
    </kap>
        <drv mrk="ventr.0o">
            <kap><ofc>*</ofc><tld/>o</kap>
            <snc mrk="ventr.0o.ANA">
                <uzo tip="fak">ANA</uzo>
                <dif>
                    Parto de la korpo, enhavanta la stomakon kaj
intestojn, inter
                    brusto
                    kaj pelvo:
                    <ekz>
                        ku&scirc;i sur la <tld/>o;
                    </ekz>
                    <ekz>
                        lia <tld/>o diki&gcirc;as
                    </ekz>
                </dif>
                <refgrp tip="sin">
                    <ref cel="abdome.0o">abdomeno</ref>.
                </refgrp>
                <refgrp tip="vid">
                    <ref cel="barel">bareli&gcirc;i</ref>.
                </refgrp>
                <trd lng="es">vientre</trd>
            </snc>
            <snc mrk="ventr.0o.malsato">
                <dif>
                    Tiu parto, konsiderata kiel sentiganta la malsaton
kaj akceptanta
                    la nutra&jcirc;ojn:
                    <ekz>
                        <ind><tld/>o malsata orelon ne havas</ind>
                        <fnt><bib>PrV</bib></fnt>;
                        <trd lng="hu">&eacute;hes embernek nincs
f&uuml;le mes&eacute;re</trd>
                    </ekz>
                    <ekz>
                        <ind>sur la <tld/>o veluro, en la <tld/>o
murmuro</ind>
                        <fnt><bib>PrV</bib></fnt>;
                        <trd lng="hu">b&aacute;rsony mell&eacute;ny,
&uuml;res has</trd>
                    </ekz>
                    <ekz>
                        liaj okuloj estas pli grandaj ol la
<tld/>o<fnt>Z</fnt>
                        <klr>(li pli avidas ol li povas
                        man&gcirc;i)</klr>;
                    </ekz>
                    <ekz>
                        tiuj, kiuj zorgas nur pri la <tld/>o<fnt>Z</fnt>
                        <klr>(materiaj, korpaj &gcirc;uoj)</klr>.
                    </ekz>
                </dif>
                <trd lng="es">tripa</trd>
                <trd lng="ru">&c_b;&c_r;&c_ju;&c_h;&c_o;</trd>
            </snc>
            <snc mrk="ventr.0o.utero">
                <dif>
                    Tiu parto konsiderata &ccirc;e la ino kiel
lo&gcirc;ejo de
                    naski&gcirc;onto:
                    <ekz>
                        ho filo de mia <tld/>o<fnt>Z</fnt>;
                    </ekz>
                    <ekz>
                        senfrukta <tld/>o<fnt>Z</fnt>.
                    </ekz>
                </dif>
                <refgrp tip="sin">
                    <ref cel="uter.0o">utero</ref>.
                </refgrp>
                <trdgrp lng="es">
                  <trd>barriga</trd>,
                  <trd>panza</trd>,
                  <trd>tripa</trd>
                </trdgrp>
                <trd lng="ru">&c_l;&c_o;&c_n;&c_o;</trd>
            </snc>
            <trdgrp lng="be">
              <trd>&c_zh;&c_y;&c_v;&c_o;&c_t;</trd>,
              <trd>&c_b;&c_r;&c_u;&c_h;&c_a;</trd>,
              <trd>&c_p;&c_u;&c_z;&c_a;</trd>,
              <trd>&c_u;&c_l;&c_o;&c_n;&c_mol;&c_n;&c_je;</trd>
            </trdgrp>
  <trd lng="ca">ventre</trd>
  <trdgrp lng="cs">
    <trd>bachor</trd>,
    <trd>b&rcaron;icho</trd>,
    <trd>&uacute;troby</trd>
  </trdgrp>
  <trd lng="de">Bauch</trd>
  <trd lng="el">&kappa;&omikron;&jota;&lambda;&jota;&alfa_ton;</trd>
  <trd lng="fr">ventre</trd>
  <trd lng="he">&bet;&tet;&fnun;</trd>
  <trd lng="hu">has</trd>
  <trd lng="id">perut</trd>
  <trdgrp lng="it">
              <trd>ventre</trd>,
              <trd>pancia<klr> (ventre)</klr></trd>
            </trdgrp>
  <trdgrp lng="ja">
    <trd>&#33145;  <pr>&#x306F;&#12425;</pr></trd>,
    <trd>&#33145;&#x90E8;  <pr>&#12405;&#x304F;&#12406;</pr></trd>
  </trdgrp>
  <trd lng="nl">buik</trd>
  <trd lng="pl">brzuch</trd>
  <trdgrp lng="pt">
                <trd>ventre</trd>,
                <trd>barriga</trd>
            </trdgrp>
  <trd lng="ru">&c_zh;&c_i;&c_v;&c_o;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>bachor</trd>,
    <trd>brucho</trd>,
    <trd>&uacute;troby</trd>
  </trdgrp>
  <trdgrp lng="sv">
              <trd>mage</trd>,
              <trd>buk</trd>
            </trdgrp>
  <trd lng="tok">insa</trd>
  <trd lng="tr">kar&inodot;n</trd>
  <trdgrp lng="uk">
    <trd>&c_zh;&c_i;&c_v;&c_ib;&c_t;</trd>,
    <trd>&c_ch;&c_je;&c_r;&c_je;&c_v;&#x43E;</trd>,
    <trd>&#x43B;&#x43E;&#x43D;&#x43E;</trd>,
    <trd>&c_u;&c_t;&c_r;&#x43E;&c_b;&c_a;</trd>
  </trdgrp>
</drv>
    </art>
