<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ezop.xml,v 1.8 2020/06/30 18:32:33 revo Exp $">
<kap>
  <rad>Ezop</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="ezop.0o">
  <kap><tld/>o</kap>
  <snc mrk="ezop.0o.PERS">
    <uzo tip="fak">PERS</uzo>
    <dif>
      Supozata <ref tip="lst" lst="voko:historiaj_personoj" cel="auxtor.0o.JUR">a&ubreve;toro</ref>
      de antikvaj grekaj <ref cel="fabl.0o">fablo</ref>j el la 7a a&ubreve; 6a jarcento 
      a.K.:
      <ekz>
        eldoni la fablojn de <tld/>o
        <fnt>
          <bib>LR</bib>
          <lok>2. La verkaro de Zamenhof</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la plej mizera kripla&jcirc;o de <tld/>o povas brili granda, aminda animo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ctl><tld/>a Sa&gcirc;o</ctl>
        <klr>(fablaro de Kalo&ccirc;ajo la&ubreve; Ezopo)</klr>
        <fnt>
          <aut>K. Kalocsay</aut>,
          <vrk>Ezopa Sa&gcirc;o</vrk>,
          <lok>Koko, 1956</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_E;&c_z;&c_o;&c_p;</trd>
  <trd lng="grc">&Alfa;&jota_psili_acute;&sigma;&omega;&pi;&omikron;&sigma_fina;</trd>
  <trd lng="cs">Ezop</trd>
  <trd lng="de">Aesop</trd>
  <trd lng="fr">&Eacute;sope</trd>
  <trd lng="it">Esopo</trd>
  <trd lng="pl">Ezop</trd>
  <trd lng="pt">Esopo</trd>
  <trd lng="sk">Ezop</trd>
</drv>
</art>

