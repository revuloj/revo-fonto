<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: klub.xml,v 1.25 2023/12/03 09:58:51 revo Exp $">
<kap><ofc>*</ofc>
<rad>klub</rad>/o</kap>
<drv mrk="klub.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="societ.0o">Societo</ref>, kiun konsistigas
      homoj kun komunaj interesoj a&ubreve; okupi&gcirc;oj politikaj,
      sciencaj,
      artaj, sportaj a&ubreve; ludaj:
      <ekz>
        tre fama loka Volapuka <tld/>o
        <fnt><aut>Pejno Simono</aut>:
        <vrk>Faligas la fundamentojn de esperantismo (Ziko Marcus
        Sikosek,
        Esperanto sen mitoj)</vrk>, <bib>Monato</bib><lok>jaro 1999a,
        numero 6a, p. 27a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_ju;&c_b;
  <klr>(&c_z;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_o;&c_ch;&c_a;&c_n;&c_mol;&c_n;&c_je;
  &c_p;&c_a;&c_v;&c_o;&c_d;&c_l;&c_je;
  &c_ib;&c_n;&c_t;&c_a;&c_r;&c_e;&c_s;&c_a;&c_w;)</klr></trd>
  <trd lng="cs">klub</trd>
  <trdgrp lng="de">
    <trd>Klub</trd>,
    <trd>Verein</trd>
  </trdgrp>
  <trd lng="en">club</trd>
  <trd lng="es">club</trd>
  <trdgrp lng="fr">
    <trd>club<klr tip="ind"> (soci&eacute;t&eacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>egyes&uuml;let</trd>,
    <trd>klub</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30AF;&#x30E9;&#x30D6;</trd>,
    <trd>&#x4F1A;  <pr>&#x304B;&#x3044;</pr></trd>,
    <trd>&#x7D50;&#x793E;  <pr>&#x3051;&#x3063;&#x3057;&#x3083;</pr></trd>
  </trdgrp>
  <trd lng="nl">club</trd>
  <trd lng="pl">klub</trd>     
  <trd lng="ro">club</trd>
  <trd lng="ru">&c_k;&c_l;&c_u;&c_b;</trd>
  <trd lng="sk">klub</trd>
  <trd lng="uk">&#x43A;&#x43B;&#x443;&#x431; (&#x43E;&#x431;&#x2019;&#x454;&#x434;&#x43D;&#x430;&#x43D;&#x43D;&#x44F;)</trd>
  <trdgrp lng="zh">
    <trd>&#x4FF1;&#x4E50;&#x90E8; <pr>j&#xF9;l&#xE8;b&#xF9;</pr></trd>,
    <trd>&#x7403;&#x7AFF; <pr>qi&#xFA;g&#x101;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="klub.0ano">
  <kap><tld/>ano</kap>
  <snc>
    <dif>
      Ano de <tld/>o, partoprenanto en <tld/>aj aran&gcirc;oj:
      <ekz>
        kelkaj niaj klubanoj <klr>[...]</klr> jam dum jaroj
        la&ubreve;tlegas kaj
        surbendigigas MONATO-n por blindaj esperantistoj
        <fnt><aut>Ru
        Bossong</aut>: <vrk>Kadi&scirc; por Jedwabne</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 5a, p. 6a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_l;&c_je;&c_c;
  &c_k;&c_l;&c_ju;&c_b;&c_u;</trd>
  <trd lng="cs">&ccaron;len klubu</trd>
  <trdgrp lng="de">
    <trd>Klubmitglied</trd>,
    <trd>Vereinsmitglied</trd>
  </trdgrp>
  <trd lng="es">miembro de club</trd>
  <trdgrp lng="fr">
    <trd>clubiste</trd>,
    <trd>membre d'un club</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>klubtag</trd>,
    <trd>egyes&uuml;leti tag</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30AF;&#x30E9;&#x30D6;&#x54E1;  <pr>&#x30AF;&#x30E9;&#x30D6;&#x3044;&#x3093;</pr></trd>,
    <trd>&#x4F1A;&#x54E1;  <pr>&#x304B;&#x3044;&#x3044;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">lid<klr tip="ind"> (v.e.club)</klr></trd>
  <trd lng="pl">klubowicz</trd>     
  <trd lng="ro">membru al clubului</trd>
  <trd lng="ru">&c_ch;&c_l;&c_je;&c_n; &c_k;&c_l;&c_u;&c_b;&c_a;</trd>
  <trd lng="sk">&ccaron;len klubu</trd>
  <trd lng="uk">&#x447;&#x43B;&#x435;&#x43D; &#x43A;&#x43B;&#x443;&#x431;&#x443;</trd>
  <trdgrp lng="zh">
    <trd>&#x6210;&#x5458; <pr>ch&#xE9;ngyu&#xE1;n</pr></trd>,
    <trd>&#x4F1A;&#x5458; <pr>hu&#xEC;yu&#xE1;n</pr></trd>,
    <trd>&#x793E;&#x5458; <pr>sh&#xE8;yu&#xE1;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="klub.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Loko, &ccirc;ambro, eventuale trinkejo, en kiu <tld/>anoj kutime
      kunvenas:
      <ekz>
        elirante el la <tld/>ejo, li senti&gcirc;is kontenta
        <fnt><aut>Renervo
        Da&scirc;o</aut>: <vrk>Ne-atenditaj sekvoj</vrk>,
        <bib>Monato</bib><lok>jaro 1999a, numero 8a, p. 17a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_ju;&c_b;
  <klr>(&c_m;&c_je;&c_s;&c_c;&c_a;)</klr></trd>
  <trd lng="cs">klubovna</trd>
  <trdgrp lng="de">
    <trd>Klubheim</trd>,
    <trd>Vereinslokal</trd>
  </trdgrp>
  <trd lng="es">club<klr> (local)</klr></trd>
  <trdgrp lng="fr">
    <trd>club<klr tip="ind"> (local)</klr></trd>
  </trdgrp>
  <trd lng="hu">klubhelyis&eacute;g</trd>
  <trdgrp lng="ja">
    <trd>&#x30AF;&#x30E9;&#x30D6;&#x5BA4;  <pr>&#x30AF;&#x30E9;&#x30D6;&#x3057;&#x3064;</pr></trd>,
    <trd>&#x30AF;&#x30E9;&#x30D6;&#x96C6;&#x4F1A;&#x6240;  <pr>&#x30AF;&#x30E9;&#x30D6;&#x3057;&#x3085;&#x3046;&#x304B;&#x3044;&#x3057;&#x3087;</pr></trd>,
    <trd>&#x4F1A;&#x9928;  <pr>&#x304B;&#x3044;&#x304B;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">clublokaal</trd>
  <trdgrp lng="pl">
    <trd>klub</trd>,
    <trd>&sacute;wietlica</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>camera de zi</trd>,
    <trd>club</trd>
  </trdgrp> 
  <trd lng="ru">&c_k;&c_l;&c_u;&c_b;<klr>
  (&c_m;&c_je;&c_s;&c_t;&c_o;)</klr></trd>
  <trd lng="sk">klubov&ncaron;a</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43B;&#x443;&#x431; (&#x43F;&#x440;&#x438;&#x43C;&#x456;&#x449;&#x435;&#x43D;&#x43D;&#x44F;)</trd>,
    <trd>&#x43F;&#x440;&#x438;&#x43C;&#x456;&#x449;&#x435;&#x43D;&#x43D;&#x44F; &#x43A;&#x43B;&#x443;&#x431;&#x443;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4FF1;&#x4E50;&#x90E8; <pr>j&#xF9;l&#xE8;b&#xF9;</pr></trd>,
    <trd>&#x4F1A;&#x6240; <pr>hu&#xEC;su&#x1D2;</pr></trd>
  </trdgrp>
</drv>
</art>
