<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>klub</rad>/o
</kap>
<drv mrk="klub.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      <ref tip="sin" cel="societ.0o">Societo</ref>, kiun konsistigas
      homoj kun komunaj interesoj a&ubreve; okupi&gcirc;oj (politikaj, sciencaj,
      artaj, sportaj ktp).
    </dif>
   <trdgrp lng="de">
     <trd>Klub</trd>,
     <trd>Verein</trd>
   </trdgrp>
  </snc>
  <snc num="2">
    <dif>
      Aristokrata <ref tip="sin" cel="societ.0o">societo</ref>
      por interparolado, amuzo ktp.  
    </dif>
  </snc>
   <trd lng="de">Club</trd>
  <trd lng="en">club</trd>
  <trd lng="nl">club</trd>
  <trd lng="ru">&c_k;&c_l;&c_u;&c_b;</trd>
</drv>
</art>
