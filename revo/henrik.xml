<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Henrik</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="henrik.0o">
  <kap><tld/>o</kap>
  <snc mrk="henrik.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira anta&ubreve;nomo &gcirc;ermandevena:
      <ekz>
        li sendas sian fraton Anglujon, al la re&gcirc;o <tld/>o
        <fnt><bib>FK</bib>, <lok>4, rakontoj, nokto, (de Eube el
        Odeso)</lok></fnt>.
      </ekz>
    </dif>
  </snc>    
  <trd lng="br">Herri</trd>
  <trd lng="de">Hendrik</trd>
  <trdgrp lng="fr">
    <trd>&Eacute;meric</trd>,
    <trd>Henri</trd>
  </trdgrp>
  <trd lng="hu">Henrik</trd>
   <trdgrp lng="it">
    <trd>Enrico</trd>
    <trd>Errico</trd>
  </trdgrp>
  <trd lng="nl">Hendrik</trd>
  <trdgrp lng="pl">
    <trd>Henryk</trd>
    <trd>Heniek<klr> (nieformalnie)</klr></trd>
  </trdgrp>
  <trd lng="pt">Henrique</trd>
  <trdgrp lng="ru">
    <trd>&c_G;&c_je;&c_n;&c_r;&c_i;&c_h;</trd>,
    <trd>&c_G;&c_je;&c_n;&c_r;&c_i;</trd>,
    <trd>&c_A;&c_n;&c_r;&c_i;</trd>
  </trdgrp>
</drv>
</art>

