<art mrk="$Id: henrik.xml,v 1.17 2023/08/31 15:46:16 revo Exp $">
<kap>
  <rad>Henrik</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="henrik.0o">
  <kap><tld/>o</kap>
  <snc mrk="henrik.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        li sendas sian fraton Anglujon, al la re&gcirc;o <tld/>o
        <fnt><bib>FK</bib>, <lok>4, rakontoj, nokto, (de Eube el
        Odeso)</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_G;&c_je;&c_n;&c_r;&c_y;&c_k;</trd>,
    <trd>&c_G;&c_je;&c_n;&c_r;&c_y;&c_h;</trd>
  </trdgrp>
  <trd lng="br">Herri</trd>
  <trdgrp lng="cs">
    <trd>Hynek</trd>,
    <trd>Jind&rcaron;ich</trd>
  </trdgrp>
  <trd lng="de">Hendrik</trd>
  <trdgrp lng="fr">
    <trd>&Eacute;meric</trd>,
    <trd>Henri</trd>
  </trdgrp>
  <trd lng="hu">Henrik</trd>
  <trdgrp lng="it">
    <trd>Enrico</trd>
    <trd>Errico</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30D8;&#x30F3;&#x30EA;&#x30FC;</trd>,
    <trd>&#x30A2;&#x30F3;&#x30EA;</trd>,
    <trd>&#x30CF;&#x30A4;&#x30F3;&#x30EA;&#x30C3;&#x30D2;</trd>
  </trdgrp>
  <trd lng="nl">Hendrik</trd>
  <trd lng="no">Henrik</trd>
  <trdgrp lng="pl">
    <trd>Henryk</trd>
    <trd>Heniek<klr> (nieformalnie)</klr></trd>
  </trdgrp>
  <trd lng="pt">Henrique</trd>
  <trdgrp lng="ru">
    <trd>&c_G;&c_je;&c_n;&c_r;&c_i;&c_h;</trd>,
    <trd>&c_G;&c_je;&c_n;&c_r;&c_i;</trd>,
    <trd>&c_A;&c_n;&c_r;&c_i;</trd>
  </trdgrp>
  <trd lng="sk">Henrych</trd>
  <trdgrp lng="uk">
    <trd>&c_G;&c_je;&#x43D;&c_r;&c_ib;&c_h;</trd>,
    <trd>&c_G;&c_je;&#x43D;&c_r;&c_i;&#x43A;</trd>
  </trdgrp>
</drv>
