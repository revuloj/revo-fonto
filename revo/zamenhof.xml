    <kap>
      <ofc>*</ofc><ofc>8</ofc>
      <rad>Zamenhof</rad>/o
    </kap>

    <drv mrk="zamenhof.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">PERS</uzo>
      <snc>
	<dif>
          &leftquot;Doktoro <ref tip="sin"
cel="espera.0o.dro">Esperanto</ref>&rightquot;,
