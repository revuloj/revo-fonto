<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>balzamin</rad>/o
</kap>
<drv mrk="balzam1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="balzam1.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Impatiens</trd>).
    </dif>
  </snc>
  <bld lok="../bld/balzamin.jpg">
    <ind>glanda <tld/>o</ind>
    <trd lng="de">dr&uuml;siges Springkraut</trd>
    <trd lng="la">Impatiens glandulifera</trd>
  </bld>
  <trd lng="de">Springkraut</trd>
  <trd lng="fr">balsamine</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_l;&c_mol;&c_z;&c_a;&c_m;&c_i;&c_n;</trd>,
    <trd>&c_n;&c_je;&c_d;&c_o;&c_t;&c_r;&c_o;&c_g;&c_a;
    &c_b;&c_a;&c_l;&c_mol;&c_z;&c_a;&c_m;&c_i;&c_n;&c_o;&c_v;&c_a;&c_ja;</trd>
  </trdgrp>
</drv>
<drv mrk="balzam1.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Balsaminaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Balsaminengew&auml;chse</trd>
  <trd lng="fr">balsaminac&eacute;es</trd>
</drv>
</art>
