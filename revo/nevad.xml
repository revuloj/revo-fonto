<art mrk="$Id: nevad.xml,v 1.10 2024/01/29 15:21:35 revo Exp $">
<kap>
  <rad>Nevad</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="nevad.0o">
  <kap><tld/>o</kap>
  <snc mrk="nevad.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Sub&scirc;tato en la okcidenta parto de
      <ref tip="malprt" cel="uson.U0o.GEOG">Usono</ref>:
      <ekz>
        la usona &scirc;tato <tld/>o ne emas akcepti nukleajn
        ruba&jcirc;ojn el la cetero de usono
        <fnt>
          <aut>G. Partlow</aut>,
          <vrk>en <url ref="https://groups.google.com/forum/#!msg/soc.culture.esperanto/AKcyXKaU6S8/_cgXY0TOzMQJ">
          Nukleruba&jcirc;o de Tajvano</url></vrk>,
          <lok>sce, 1997-01-26</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_N;&c_e;&c_v;&c_a;&c_d;&c_a;</trd>
  <trd lng="cs">Nevada</trd>
  <trd lng="en">Nevada</trd>
  <trd lng="fr">N&eacute;vada</trd>
  <trd lng="ja">&#x30CD;&#x30D0;&#x30C0;&#x5DDE;  <pr>&#x306D;&#x3070;&#x3060;&#x3057;&#x3085;&#x3046;</pr></trd>
  <trd lng="nl">Nevada</trd>
  <trd lng="no">Nevada</trd>
  <trd lng="pl">Nevada</trd>
  <trd lng="ro">Nevada</trd>
  <trd lng="sk">Nevada</trd>
  <trd lng="uk">&#x41D;&c_je;&c_v;&c_a;&c_d;&c_a; (&c_sh;&c_t;&c_a;&c_t;)</trd>
  <trdgrp lng="zh">
    <trd>&#x5167;&#x83EF;&#x9054; <pr>N&#xE8;ihu&#xE1;d&#xE1;</pr></trd>,
    <trd>&#x5185;&#x534E;&#x8FBE; <pr>N&#xE8;ihu&#xE1;d&#xE1;</pr></trd>,
    <trd>&#x5167;&#x83EF;&#x9054;&#x5DDE; <pr>N&#xE8;ihu&#xE1;d&#xE1;zh&#x14D;u</pr></trd>,
    <trd>&#x5185;&#x534E;&#x8FBE;&#x5DDE; <pr>N&#xE8;ihu&#xE1;d&#xE1;zh&#x14D;u</pr></trd>
  </trdgrp>
</drv>
