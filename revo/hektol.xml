<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>hektolitr</rad>/o
</kap>
<drv mrk="hektol.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Praktika <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de
      kapacito,
      egala al cent <ref tip="prt" cel="litr.0o">litroj</ref>; simb. hl.
    </dif>
  </snc>
  <trd lng="de">Hektoliter</trd>
  <trd lng="fr">hectolitre</trd>
  <trd lng="nl">hectoliter</trd>
  <trd lng="ru">&c_g;&c_je;&c_k;&c_t;&c_o;&c_l;&c_i;&c_t;&c_r;</trd>
</drv>
</art>
