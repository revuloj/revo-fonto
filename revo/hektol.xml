<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hektol.xml,v 1.21 2022/03/20 15:11:17 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>hektolitr</rad>/o
</kap>
<drv mrk="hektol.0o">
  <kap>
    <tld/>o
  </kap>
  <mlg>hl</mlg>
  <snc>
    <dif>
      Praktika <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de
      kapacito, egala al cent <ref tip="prt" cel="litr.0o">litroj</ref>:
      <ekz>
        la intervalo inter la el&scirc;prucoj, kiuj da&ubreve;ras &ccirc;irka&ubreve; 25 minutojn kaj liveras 600
        <tld/>ojn da mineralakvo, estas 36&#x202F;-&#x202F;38 horoj
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2012/010472.php">Varmaj vortoj por malvarmaj akvoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>                          
  <trd lng="be">&c_g;&c_je;&c_k;&c_t;&c_a;&c_l;&c_ib;&c_t;&c_a;&c_r;</trd>
  <trd lng="bg">&c_h;&c_je;&c_k;&c_t;&c_o;&c_l;&c_i;&c_t;&c_malmol;&c_r;</trd>
  <trd lng="br">hektolitr(ad)</trd>
  <trd lng="ca">hectolitre</trd>
  <trd lng="de">Hektoliter</trd>
  <trd lng="el">&epsilon;&kappa;&alfa;&tau;&omikron_ton;&lambda;&jota;&tau;&rho;&omikron;</trd>
  <trdgrp lng="en">
    <trd>hectoliter</trd>,
    <trd>hectolitre</trd>
  </trdgrp>
  <trd lng="es">hectolitro</trd>
  <trd lng="fr">hectolitre</trd>
  <trd lng="hu">hektoliter</trd>
  <trd lng="it">ettolitro</trd>
  <trd lng="nl">hectoliter</trd>
  <trd lng="pt">hectolitro</trd>
  <trd lng="ru">&c_g;&c_je;&c_k;&c_t;&c_o;&c_l;&c_i;&c_t;&c_r;</trd>
  <trd lng="uk">&#x433;&#x435;&#x43A;&#x442;&#x43E;&#x43B;&#x456;&#x442;&#x440;</trd>
</drv>
</art>
