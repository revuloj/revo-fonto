    </dif>
  </snc>
  <trd lng="de">Hektoliter</trd>
  <trd lng="fr">hectolitre</trd>
  <trd lng="ru">&c_g;&c_je;&c_k;&c_t;&c_o;&c_l;&c_i;&c_t;&c_r;</trd>
</drv>
</art>
