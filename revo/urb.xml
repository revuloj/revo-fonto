       malri&ccirc;ularo:
       <ekz>
	 la surlokaj kuracistoj en la Karakasaj lad<tld/>oj jam plendas ke
	 ili ne plu ricevas laboron
	 <fnt>
	 <aut>J. Galtung, trad. V. L.</aut>,
	 <vrk><url
	 ref="http://mas-eo.org/article.php3?id_article=89">Davoso
	 a&ubreve; Portalegro</url></vrk>,
	 <lok>Monda Asembleo Socia, 2006-03-31</lok>
	 </fnt>
       </ekz>
     </dif>
   </snc>
   <trd lng="ca">barri de <ind>barracots</ind></trd>
   <trd lng="de">Slum</trd>
   <trd lng="en">slum</trd>
   <trd lng="es">barrio de <ind>chabolas</ind></trd>
   <trd lng="fa">&a_H;&a_l;&a_b;&f_y;&zwnj;&a_A_madda;&a_b;&a_A;&a_d;</trd>
   <trd lng="fi">h&ouml;kkelikaupunki</trd>
   <trd lng="fr">bidonville</trd>
   <trd lng="hu">b&aacute;dogv&aacute;ros</trd>
   <trdgrp lng="it">
     <trd>baraccopoli</trd>, 
     <trd>bidonville</trd>, 
     <trd>slum</trd>
   </trdgrp>
</drv>

<drv mrk="urb.milion0o">
  <kap>milion<tld/>o</kap>
  <snc mrk="urb.milion0o.ARKI">
    <dif>
      <tld lit="U"/>o kun almena&ubreve; unu miliono (1000000) da lo&gcirc;antoj:
      <ekz>
        <nom>Ni&jcirc;nij Novgorod</nom> i&gcirc;is milion<tld/>o fine de la 1960-aj
