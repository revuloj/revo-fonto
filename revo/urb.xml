     <trd>extraradi</trd>
   </trdgrp>
   <trdgrp lng="es">
     <trd>afueras</trd>,
     <trd>extraradio</trd>
   </trdgrp>
   <trdgrp lng="fi">
     <trd>ymp&auml;rist&ouml;kaupunki</trd>,
     <trd>l&auml;hikaupunki</trd>
   </trdgrp>
   <trd lng="fr">ville de banlieue</trd>
   <trd lng="hu">peremv&aacute;ros</trd>
   <trd lng="it">periferia<klr> (urbana)</klr></trd>
   <trd lng="pt">periferia</trd>
   <trd lng="ru">&c_g;&c_o;&c_r;&c_o;&c_d;-&c_s;&c_p;&c_u;&c_t;&c_n;&c_i;&c_k;</trd>
