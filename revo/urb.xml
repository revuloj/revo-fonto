     <dif>
       Parto de <ref tip="malprt" cel="urb.0o.ARKI"><tld/>o</ref> ekster
       la &ccirc;irka&ubreve;murego a&ubreve;
       &ccirc;irka&ubreve;bulvardoj; ekstera parto de <tld/>o:
       <ekz>
	 komence <klr>....</klr> la elektron aran&gcirc;u, la akvon 
donu al la
	 anta&ubreve;<tld/>oj
	 <fnt><bib>Metrop</bib></fnt>!
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_p;&c_r;&c_a;&c_d;&c_m;&c_je;&c_s;&c_mol;&c_c;&c_je;</trd>,
     <trd>&c_p;&c_r;&c_y;&c_g;&c_a;&c_r;&c_a;&c_d;</trd>
   </trdgrp>
   <trdgrp lng="ca">
     <trd>suburbi</trd>,
     <trd>raval</trd>
   </trdgrp>
   <trd lng="cs">p&rcaron;edm&ecaron;st&iacute;</trd>
   <trd lng="de">Vorstadt</trd>
   <trd lng="en">suburb</trd>
