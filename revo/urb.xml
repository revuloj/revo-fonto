<drv mrk="urb.cxef0o">
   <kap>&ccirc;ef<tld/>o</kap>
   <snc mrk="urb.cxef0o.POL">
     <uzo tip="fak">POL</uzo>
     <dif>
       <ref tip="super" cel="urb.0o.ARKI"><tld lit="U"/>o</ref>, en
       kiu trovi&gcirc;as la registara sidejo de lando, &scirc;tato,
       provinco a&ubreve; similtipaj teritorioj:
       <ekz>
         en la venonta jaro mi veturigos mian fileton al la
         &ccirc;ef<tld/>o
         <fnt><bib>Revizoro</bib>, <lok>akto 5a, sceno 7a</lok></fnt>;
       </ekz>
       <ekz>
         Moskvo estas la &ccirc;ef<tld/>o de Rusujo.
       </ekz>
     </dif>
   </snc>
   <snc mrk="urb.cxef0o.FIG">
   <uzo tip="stl">FIG</uzo>
     <dif>
       <ref tip="super" cel="urb.0o.ARKI"><tld lit="U"/>o</ref> pli
       grava ol aliaj pro iu elstariga trajto:
       <ekz>
         ekonomia, arta, kultura &ccirc;ef<tld/>o.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_s;&c_t;&c_a;&c_l;&c_ib;&c_c;&c_a;</trd>
   <trd lng="ca">capital</trd>
   <trd lng="cs">hlavn&iacute; m&ecaron;sto</trd>
   <trd lng="de">Hauptstadt</trd>
   <trd 
lng="el">&pi;&rho;&omega;&tau;&epsilon;&ypsilon_ton;&omikron;&ypsilon;&sigma;&alfa;</trd>
   <trd lng="en">capital (city)</trd>
   <trd lng="es">capital</trd>
   <trd lng="fa">&f_p;&a_A;&f_y;&a_t;&a_kh;&a_t;</trd>
   <trd lng="fi">p&auml;&auml;kaupunki</trd>
   <trd lng="fr">capitale<klr tip="amb"> (ville)</klr></trd>
   <trd lng="hu">f&odblac;v&aacute;ros</trd>
   <trd lng="id"><ind>ibu</ind> kota</trd>
   <trdgrp lng="it">
     <trd>capoluogo</trd>, 
     <trd>capitale</trd>
   </trdgrp>
   <trd lng="nl">hoofdstad</trd>
   <trd lng="pt">capital</trd>
   <trd lng="ru">&c_s;&c_t;&c_o;&c_l;&c_i;&c_c;&c_a;</trd>
   <trd lng="tr">ba&scedil;kent</trd>
   <trd lng="tt">&c_b;&c_a;&c_sh;&c_k;&c_a;&c_l;&c_a;</trd>
   <trd lng="sv">huvudstad</trd>
   <trd lng="sk">hlavn&eacute; mesto</trd>
</drv>

