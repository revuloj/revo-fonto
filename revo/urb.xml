<drv mrk="urb.0ano">
  <kap><ofc>*</ofc><tld/>ano</kap>
  <snc mrk="urb.0ano.KOMUNE">
    <dif>
      Lo&gcirc;anto de <tld/>o:
      <ekz>
	<tld/>anoj estas ordinare pli ruzaj, ol vila&gcirc;anoj
	<fnt><bib>F</bib>, <lok>&FE; 37</lok></fnt>;
      </ekz>
      <ekz>
        maljuna <tld/>ano haltis, atente rigardis lin kaj poste foriris
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>anoj tie kantis &ccirc;e sia glaso da vino
        <fnt>
          <bib>Fab2</bib>
          <lok>Sonorilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ne vidas, kion kuiras por la tagman&gcirc;o iu kamparano a&ubreve; <tld/>ano
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la prudentaj <tld/>anoj estis mirigitaj per tia konduto de l' pastra partio
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia sam<tld/>ano
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
          <lok>A&ubreve;tunovesper'</lok>
        </fnt>;
      </ekz>
      <ekz>
        por samregnanoj kaj sam<tld/>anoj la rolon de lingvo ne&ubreve;trala povas ludi la
        lingvo <klr>[&#x2026;]</klr>, kiun parolas la plimulto de la lokaj lo&gcirc;antoj
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Homaranismo</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_r;&c_a;&c_d;&c_zh;&c_a;&c_n;&c_ib;&c_n;</trd>,
    <trd>&c_m;&c_ja;&c_s;&c_t;&c_o;&c_v;&c_je;&c_c;</trd>
  </trdgrp>
  <trdgrp lng="ca">
     <trd>ciutad&agrave;</trd>,
     <trd>urbanita</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>m&ecaron;&scaron;&tcaron;an</trd>,
    <trd>obyvatel m&ecaron;sta</trd>
  </trdgrp>
  <trd lng="de">St&auml;dter</trd>
  <trd lng="es">ciudadano<klr tip="amb"> (subst.)</klr></trd>
  <trd lng="fa">&a_sh;&a_h;&a_r;&a_n;&a_sh;&f_y;&a_n;</trd>
  <trd lng="fi">kaupunkilainen</trd>
  <trd lng="fr">citadin<klr tip="amb"> (subst.)</klr></trd>
  <trdgrp lng="hu">
     <trd>v&aacute;rosi<klr tip="amb"> (lakos)</klr></trd>,
     <trd>v&aacute;roslak&oacute;</trd>
   </trdgrp>
  <trd lng="id">urban</trd>
  <trd lng="it">cittadino<klr> (di citt&agrave;)</klr></trd>
