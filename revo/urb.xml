<drv mrk="urb.cxirkaux0o">
  <kap>&ccirc;irka&ubreve;<tld/>o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="urb.cxirkaux0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      La ringo el anta&ubreve;urboj &ccirc;irka&ubreve; granda urbo apud &gcirc;ia
      <ref cel="perife.0o.KOMUNE">periferio</ref>:
      <ekz>
        <klr>[la partio]</klr> atingis en la lastaj jaroj firman grundon en Katalunio,
        &ccirc;efe en la periferiaj kvartaloj barcelonaj kaj la &ccirc;irka&ubreve;<tld/>o
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Monato;/2003/008144.html">Surprizaj rezultoj</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Abram Zies</nom>, 33-jara&gcirc;a, el la &ccirc;irka&ubreve;<tld/>o de la teksasa urbo Dallas
        <fnt>
          <bib>Monato</bib>
          <aut>AMik</aut>
          <vrk><url ref="&Monato;/2012/010469.php">Bran&ccirc;o nova</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
   <trd lng="be">&c_g;&c_o;&c_r;&c_a;&c_d;-&c_s;&c_p;&c_a;&c_d;&c_a;&c_r;&c_o;&c_zh;&c_n;&c_ib;&c_k;</trd>
