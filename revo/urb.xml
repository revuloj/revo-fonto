<drv mrk="urb.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="urb.0o.ARKI">
     <uzo tip="fak">ARKI</uzo>
     <dif>
       Iom granda aro da domoj, aran&gcirc;itaj la&ubreve; pluraj 
       stratoj, en iuj lokoj kaj epokoj
       &ccirc;irka&ubreve;ita de murego:
       <ekz>
	 mi konas neniun en tiu &ccirc;i <tld/>o
	 <fnt><bib>F</bib>, <lok>&FE; 28</lok></fnt>;
       </ekz>
       <ekz>
	 &gcirc;i plu ne estos <tld/>o, sed amaso da 
         ruina&jcirc;oj<fnt>Z</fnt>;
       </ekz>
       <ekz>
	 fortika <tld/>o<fnt>Z</fnt>.
       </ekz>
     </dif>
     <refgrp tip="sub">
       <ref cel="burg.0o.ARKI">burgo</ref>,
       <ref cel="civit.0o.urbo">civito</ref>
     </refgrp>
   </snc>
  <trdgrp lng="be">
     <trd>&c_g;&c_o;&c_r;&c_a;&c_d;</trd>,
     <trd>&c_m;&c_je;&c_s;&c_t;&c_a;</trd>
   </trdgrp>
  <trd lng="bg">&c_g;&c_r;&c_a;&c_d;</trd>
  <trd lng="br">k&ecirc;r</trd>
  <trd lng="ca">ciutat</trd>
  <trd lng="cs">m&ecaron;sto</trd>
  <trd lng="de">Stadt</trd>
  <trd lng="el">&pi;&omikron_ton;&lambda;&eta;</trd>
  <trd lng="en">city</trd>
  <trd lng="es">ciudad</trd>
  <trd lng="fa">&a_sh;&a_h;&a_r;</trd>
  <trd lng="fi">kaupunki</trd>
  <trd lng="fr">ville</trd>
  <trd lng="hu">v&aacute;ros</trd>
  <trd lng="id">kota</trd>
  <trd lng="it">citt&agrave;</trd>
  <trd lng="ku">bajar</trd>
  <trd lng="nl">stad</trd>
  <trd lng="pl">miasto</trd>
  <trd lng="pt">cidade</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_o;&c_d;</trd>
  <trd lng="sk">mesto</trd>
  <trd lng="sv">stad</trd>
  <trd lng="tt">&c_sh;&#1241;&#1211;&#1241;&c_r;</trd>
  <trd lng="tr">&scedil;ehir</trd>
  <trd lng="vo">zif</trd>
  <trdgrp lng="zh">
     <trd>&#24066; [<ind>sh&igrave;</ind>]</trd>
     <trd>&#22478;&#24066; [<ind>ch&eacute;ng sh&igrave;</ind>]</trd>
     <trd>&#37117;&#24066; [<ind>d&#363; sh&igrave;</ind>]</trd>
   </trdgrp>
</drv>

