<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>urb</rad>/o
</kap>
<drv mrk="urb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="urb.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Iom granda aro da domoj, aran&gcirc;itaj la&ubreve; pluraj stratoj, iam (historie)
      &ccirc;irka&ubreve;ita de murego:
      <ekz>
        mi konas neniun en tiu &ccirc;i <tld/>o
        <fnt><bib>F</bib>, <lok>&FE; 28</lok></fnt>;
      </ekz>
      <ekz>
        &gcirc;i plu ne estos <tld/>o, sed amaso da ruina&jcirc;oj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        fortika <tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="civit.0o.urbo">civito</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_o;&c_r;&c_a;&c_d;</trd>,
    <trd>&c_m;&c_je;&c_s;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="br">k&ecirc;r</trd>
  <trd lng="cs">m&ecaron;sto</trd>
  <trd lng="de">Stadt</trd>
  <trd lng="el">&pi;&omikron_ton;&lambda;&eta;</trd>
  <trd lng="en">city</trd>
  <trd lng="es">ciudad</trd>
  <trd lng="fr">ville</trd>
  <trd lng="hu">v&aacute;ros</trd>
  <trd lng="it">citt&agrave;</trd>
  <trd lng="nl">stad</trd>
  <trd lng="pl">miasto</trd>
  <trd lng="pt">cidade</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_o;&c_d;</trd>
  <trd lng="tr">&scedil;ehir</trd>
  <trd lng="sv">stad</trd>
  <trd lng="sk">mesto</trd>
  <trd lng="vo">zif</trd>
</drv>

<drv mrk="urb.0ano">
  <kap><ofc>*</ofc><tld/>ano</kap>
  <snc mrk="urb.0ano.KOMUNE">
    <dif>
      Lo&gcirc;anto de <tld/>o:
      <ekz>
        <tld/>anoj estas ordinare pli ruzaj, ol vila&gcirc;anoj
        <fnt><bib>F</bib>, <lok>&FE; 37</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">citadin<klr tip="amb"> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>v&aacute;rosi<klr tip="amb"> (lakos)</klr></trd>,
    <trd>v&aacute;roslak&oacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_o;&c_zh;&c_a;&c_n;&c_i;&c_n;</trd>
</drv>

<drv mrk="urb.U0ano">
  <kap><tld lit="U"/>ano</kap>
  <snc mrk="urb.U0ano.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira anta&ubreve;nomo latindevena:
      <ekz>
        la papoj <tld lit="U"/>ano VIII <klr>(la 8a)</klr> kaj Innocento IV <klr>(la
        4a)</klr>
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj,
        bagateloj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Urban</trd>
  <trd lng="fr">Urbain<klr tip="amb"> (pr&eacute;nom)</klr></trd>
</drv>

<drv mrk="urb.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="urb.0ego.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Tre granda <ref tip="super"
      cel="urb.0o.ARKI"><tld/></ref>okomplekso, ofte entenanta plurajn
      eks<tld/>ojn:
      <ekz>
        en la <tld/>egoj, &ccirc;iuj (neri&ccirc;uloj) lo&gcirc;as en anta&ubreve;<tld/>oj kaj
        &ccirc;iutage iras longan distancon buse a&ubreve; metroe
        <fnt>
        <aut>B&aring;rd Hekland</aut>,
        <vrk>Kompreni &ccirc;i-landon? Neniam!</vrk>,
        <bib>LOdE</bib>,
        <lok>numero 53 (1999:3)</lok>
        </fnt>;
      </ekz>
      <ekz>
        la plej <ctl>delegitoza</ctl> <tld/>o plu restas la
        brazila <tld/>ego Rio de Janeiro
        <fnt>
        <vrk>Preska&ubreve; du mil delegitoj</vrk>,
        <bib>LOdE</bib>, <lok>numero 71 (2000:8-9)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">velkom&ecaron;sto</trd>
  <trd lng="de">Gro&szlig;stadt</trd>
  <trd lng="en">megalopolis</trd>
  <trd lng="fr">m&eacute;galopole</trd>
  <trdgrp lng="hu">
    <trd>nagyv&aacute;ros</trd>,
    <trd>metropolisz</trd>
  </trdgrp>
  <trd lng="nl">grootstad</trd>
  <trd lng="ru">&c_m;&c_je;&c_g;&c_a;&c_p;&c_o;&c_l;&c_i;&c_s;</trd>
  <trd lng="vo">lezif</trd>
</drv>

<drv mrk="urb.0estro">
  <kap><tld/>estro</kap>
  <snc mrk="urb.0estro.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Precipa administranto de <tld/>o.
    </dif>
    <refgrp tip="vid">
      <ref cel="skaben.0o">skabeno</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_r;&c_a;&c_d;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_g;&c_a;&c_l;&c_a;&c_v;&c_a;</trd>,
    <trd>&c_v;&c_o;&c_j;&c_t;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_m;&c_ib;&c_s;&c_t;&c_r;</trd>
  </trdgrp>
  <trd lng="de">B&uuml;rgermeister</trd>
  <trd lng="fr">maire</trd>
  <trd lng="hu">polg&aacute;rmester</trd>
  <trd lng="nl">burgemeester</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_a;&c_d;&c_o;&c_n;&c_a;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_g;&c_o;&c_r;&c_o;&c_d;&c_s;&c_k;&c_o;&c_j; &c_g;&c_o;&c_l;&c_o;&c_v;&c_a;</trd>,
    <trd>&c_m;&c_e;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="urb.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="urb.0eto.ARKI">
    <dif>
      Tre granda vila&gcirc;o a&ubreve; tre malgranda <tld/>o:
      <ekz>
        malgranda <tld/>eto kun ru&gcirc;aj tegmentoj sur la malaltaj
        domoj
        <fnt>
        <bib>Fab4</bib>, <lok>du fratoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">m&ecaron;ste&ccaron;ko</trd>
  <trd lng="de">St&auml;dtchen</trd>
  <trdgrp lng="fr">
    <trd>bourg</trd>,
    <trd>petite ville</trd>
  </trdgrp>
  <trd lng="hu">kisv&aacute;ros</trd>
  <trd lng="nl">stadje</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_r;&c_o;&c_d;&c_o;&c_k;</trd>,
    <trd>&c_m;&c_je;&c_s;&c_t;&c_je;&c_ch;&c_k;&c_o;</trd>
  </trdgrp>
  <trd lng="vo">zifil</trd>
</drv>

<drv mrk="urb.antaux0o">
  <kap><ofc>*</ofc>anta&ubreve;<tld/>o</kap>
  <snc mrk="urb.antaux0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Parto de <ref tip="malprt" cel="urb.0o.ARKI"><tld/>o</ref> ekster la
      &ccirc;irka&ubreve;murego a&ubreve; &ccirc;irka&ubreve;bulvardoj; ekstera parto de <tld/>o:
      <ekz>
        komence <klr>....</klr> la elektron aran&gcirc;u, la akvon donu al la
        anta&ubreve;<tld/>oj
        <fnt><bib>Metrop</bib></fnt>!
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_d;&c_m;&c_je;&c_s;&c_mol;&c_c;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_g;&c_a;&c_r;&c_a;&c_d;</trd>
  </trdgrp>
  <trd lng="cs">p&rcaron;edm&ecaron;st&iacute;</trd>
  <trd lng="de">Vorstadt</trd>
  <trd lng="en">suburb</trd>
  <trdgrp lng="fr">
    <trd>banlieue</trd>,
    <trd>faubourg</trd>
  </trdgrp>
  <trd lng="hu">el&odblac;v&aacute;ros</trd>
  <trd lng="nl">voorstad</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_g;&c_o;&c_r;&c_o;&c_d;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_m;&c_je;&c_s;&c_t;&c_mol;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">predmestie</trd>
  <trd lng="pt">sub&uacute;rbio</trd>
</drv>

<drv mrk="urb.cxirkaux0o">
  <kap>&ccirc;irka&ubreve;<tld/>o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="urb.cxirkaux0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      &Ccirc;iu el la <tld/>oj, situantaj en &ccirc;irka&ubreve;a&jcirc;o de granda <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">ville de banlieue</trd>
  <trd lng="hu">peremv&aacute;ros</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_o;&c_d;-&c_s;&c_p;&c_u;&c_t;&c_n;&c_i;&c_k;</trd>
</drv>

<drv mrk="urb.cxef0o">
  <kap>&ccirc;ef<tld/>o</kap>
  <snc mrk="urb.cxef0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      &Ccirc;efa <ref tip="super" cel="urb.0o.ARKI"><tld/>o</ref> de provinco a&ubreve;
      regno:
      <ekz>
        en la venonta jaro mi veturigos mian fileton al la &ccirc;ef<tld/>o
        <fnt><bib>Revizoro</bib>, <lok>akto 5a, sceno 7a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_l;&c_ib;&c_c;&c_a;</trd>
  <trd lng="cs">hlavn&iacute; m&ecaron;sto</trd>
  <trd lng="de">Hauptstadt</trd>
  <trd lng="el">&pi;&rho;&omega;&tau;&epsilon;&ypsilon_ton;&omikron;&ypsilon;&sigma;&alfa;</trd>
  <trd lng="en">capital (city)</trd>
  <trd lng="es">capital</trd>
  <trd lng="fr">capitale<klr tip="amb"> (ville)</klr></trd>
  <trd lng="hu">f&odblac;v&aacute;ros</trd>
  <trd lng="it">capoluogo</trd>
  <trd lng="nl">hoofdstad</trd>
  <trd lng="pt">capital</trd>
  <trd lng="ru">&c_s;&c_t;&c_o;&c_l;&c_i;&c_c;&c_a;</trd>
  <trd lng="tr">ba&scedil;kent</trd>
  <trd lng="sv">huvudstad</trd>
  <trd lng="sk">hlavn&eacute; mesto</trd>
</drv>
</art>
