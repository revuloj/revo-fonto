<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
 <ofc>*</ofc>
 <rad>urb</rad>/o
</kap>
<drv mrk="urb.0o">
 <kap><ofc>*</ofc><tld/>o</kap>
 <snc>
 <dif>
  Iom granda aro da domoj, aran&gcirc;itaj la&ubreve; pluraj stratoj k kelkfoje limita per &ccirc;irka&ubreve;muroj a&ubreve; barieroj: 
  <ekz>&gcirc;i plu ne estos <tld/>o, sed amaso da ruina&jcirc;oj<fnt>Z</fnt>;</ekz> 
  <ekz>fortik(igit)a <tld/>o<fnt>Z</fnt>;</ekz> 
  <ekz>
    <ind><tld/>estro</ind><klr>(administranto de <tld/>o)</klr>;
    <trdgrp lng="be">
      <trd>&c_g;&c_a;&c_r;&c_a;&c_d;&c_a;&c_v;&c_y;</trd>, 
      <trd>&c_g;&c_a;&c_l;&c_a;&c_v;&c_a;</trd>, 
      <trd>&c_v;&c_o;&c_j;&c_t;</trd>, 
      <trd>&c_b;&c_u;&c_r;&c_m;&c_ib;&c_s;&c_t;&c_r;</trd>
    </trdgrp>
  </ekz> 
  <ekz>
    <ind><tld/>odomo</ind><klr>(administrejo de <tld/>o)</klr>.
    <trd lng="be">&c_r;&c_a;&c_t;&c_u;&c_sh;&c_a;</trd>
  </ekz> 
 </dif>
 <refgrp tip="sin">
  <ref cel="civit.0o">civito</ref>.
 </refgrp>
 </snc>
 <trdgrp lng="be">
  <trd>&c_g;&c_o;&c_r;&c_a;&c_d;</trd>, 
  <trd>&c_m;&c_je;&c_s;&c_t;&c_a;</trd>
 </trdgrp>
 <trd lng="cs">m&ecaron;sto</trd>
 <trd lng="de">Stadt</trd>
 <trd lng="en">city</trd>
 <trd lng="fr">ville</trd>
 <trd lng="nl">stad</trd>
 <trd lng="pl">miasto</trd>
 <trd lng="ru">&c_g;&c_o;&c_r;&c_o;&c_d;</trd>
 <trd lng="tr">&scedil;ehir</trd>
 <trd lng="sv">stad</trd>
</drv>
<drv mrk="urb.antaux0o">
 <kap><ofc>*</ofc>anta&ubreve;<tld/>o</kap>
 <snc>
 <dif>
  Parto de <tld/>o ekster la &ccirc;irka&ubreve;muro a&ubreve; &ccirc;irka&ubreve;bulvardoj; ekstera parto de <tld/>o.  
 </dif>
 </snc>
 <trdgrp lng="be">
   <trd>&c_p;&c_r;&c_a;&c_d;&c_m;&c_je;&c_s;&c_mol;&c_c;&c_je;</trd>, 
   <trd>&c_p;&c_r;&c_y;&c_g;&c_a;&c_r;&c_a;&c_d;</trd> 
 </trdgrp>
 <trd lng="de">Vorstadt</trd>
 <trdgrp lng="fr">
   <trd>faubourg</trd>,
   <trd>banlieue</trd>
 </trdgrp>
 <trd lng="nl">voorstad</trd>
 <trdgrp lng="ru">
   <trd>&c_p;&c_r;&c_i;&c_g;&c_o;&c_r;&c_o;&c_d;</trd>,
   <trd>&c_p;&c_r;&c_je;&c_d;&c_m;&c_je;&c_s;&c_t;&c_mol;&c_je;</trd>
 </trdgrp>
</drv>
<drv mrk="urb.cxef0o">
 <kap>&ccirc;ef<tld/>o</kap>
 <snc>
 <dif>
  &Ccirc;efa <tld/>o de provinco a&ubreve; regno.  
 </dif>
 </snc>
 <trd lng="be">&c_s;&c_t;&c_a;&c_l;&c_ib;&c_c;&c_a;</trd>
 <trd lng="de">Hauptstadt</trd>
 <trd lng="fr">capitale</trd>
 <trd lng="nl">hoofdstad</trd>
 <trd lng="ru">&c_s;&c_t;&c_o;&c_l;&c_i;&c_c;&c_a;</trd>
 <trd lng="tr">ba&scedil;kent</trd>
 <trd lng="sv">huvudstad</trd>
</drv>
</art>
