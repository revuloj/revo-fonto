<drv mrk="urb.U0ano">
  <kap><tld lit="U"/>ano</kap>
  <snc mrk="urb.U0ano.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
	 la papoj <tld lit="U"/>ano VIII <klr>(la 8a)</klr> kaj Innocento
	 IV <klr>(la 4a)</klr>
	 <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj,
	 bagateloj</lok></fnt>;
      </ekz>
      <ekz>
        salutu <tld lit="U"/>anon, nian kunlaboranton por Kristo
        <fnt>
          <bib>NT</bib>
          <lok>Romanoj 16:9</lok>
        </fnt>;.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">Urb&agrave;</trd>
  <trd lng="cs">Urban</trd>
  <trd lng="de">Urban</trd>
  <trd lng="es">Urbano</trd>
  <trd lng="fa">&a_A;&a_w;&a_r;&a_b;&a_A;&a_n;</trd>
  <trd lng="fi">Urbanus</trd>
  <trd lng="fr">Urbain<klr tip="amb"> (pr&eacute;nom)</klr></trd>
  <trd lng="hu">Orb&aacute;n</trd>
  <trd lng="id">Urbanus</trd>
  <trd lng="it">Urbano</trd>
  <trdgrp lng="sk">
    <trd>me&scaron;&tcaron;an</trd>,
    <trd>obyvate&lcaron; mesta</trd>
  </trdgrp>
  <trd lng="uk">&c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol; &c_m;&c_ib;&c_s;&c_t;&c_a;</trd>
</drv>

<drv mrk="urb.0ego">
   <kap><tld/>ego</kap>
   <snc mrk="urb.0ego.ARKI">
     <uzo tip="fak">ARKI</uzo>
     <dif>
       Tre granda <ref tip="super" cel="urb.0o.ARKI"><tld/></ref>okomplekso, 
       kutime ampleksanta plurajn kunkreskintajn <tld/>ojn:
      <ekz>
        verdire la <tld/>ego <nom>Babel</nom> kun sia brua interpu&scirc;i&gcirc;ado kaj kun sia idolkulto ne
        allogas min
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        la luno <klr>[&#8230;]</klr> &scirc;ajnis rigardi kun miro la pereon de la mondreganta
        <tld/>ego <klr>[&#8230;]</klr>, Romo, kvaza&ubreve; grandega brul&scirc;tiparo, lumigis la tutan
        Kampanion
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi tuj iru al Stokholmo por intervjui nian agenton en tiu <tld/>ego
        <fnt>
          <bib>KKl</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> poste iris rekte al Berlino, kiun <tld/>egon ili atingis sen
        iaj notindaj spertoj
        <fnt>
          <bib>KKl</bib>
        </fnt>;
      </ekz>
      <ekz>
	 en la <tld/>egoj, &ccirc;iuj (neri&ccirc;uloj) lo&gcirc;as en 
         anta&ubreve;<tld/>oj kaj &ccirc;iutage iras longan distancon buse a&ubreve; metroe
	 <fnt>
    	   <aut>B. Hekland</aut>,
    	   <vrk>Kompreni &ccirc;i-landon? Neniam!</vrk>,
    	   <bib>LOdE</bib>,
    	   <lok>1999:3, numero 53a</lok>
	 </fnt>;
      </ekz>
      <ekz>
	 la plej <ctl>delegitoza</ctl> <tld/>o plu restas la
	 brazila <tld/>ego <nom>Rio de Janeiro</nom>
	 <fnt>
	   <vrk>Preska&ubreve; du mil delegitoj</vrk>,
	   <bib>LOdE</bib>,
	   <lok>2000:8-9, numero 71a</lok>
	 </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
       <ref cel="aglome.0ajxo">aglomera&jcirc;o</ref>,
       <ref cel="metrop.0o.aglomerajxo">metropolo<sncref/></ref>,
       <ref cel="urb.milion0o">milionurbo</ref>
    </refgrp>
    <ref tip="vid" cel="civit.0o.urbo">civito<sncref/></ref>
  </snc>
   <trdgrp lng="be">
     <trd>&c_g;&c_o;&c_r;&c_a;&c_d; <klr>(&c_v;&c_ja;&c_l;&c_ib;&c_k;&c_ib;)</klr></trd>,
     <trd>&c_m;&c_e;&c_g;&c_a;&c_p;&c_o;&c_l;&c_ib;&c_s;</trd>
   </trdgrp>
   <trd lng="bg">&c_g;&c_o;&c_l;&c_ja;&c_m; <ind>&c_g;&c_r;&c_a;&c_d;</ind></trd>
   <trd lng="ca">metr&ograve;poli</trd>
   <trd lng="cs">velkom&ecaron;sto</trd>
   <trd lng="de">Gro&szlig;stadt</trd>
   <trd lng="en">megalopolis</trd>
   <trd lng="es">megal&oacute;polis</trd>
   <trd lng="fa">&f_k;&a_l;&a_A;&a_n;&zwnj;&a_sh;&a_h;&a_r;</trd>
   <trd lng="fi">suurkaupunki</trd>
   <trd lng="fr">m&eacute;galopole</trd>
   <trdgrp lng="hu">
     <trd>nagyv&aacute;ros</trd>,
     <trd>metropolisz</trd>
   </trdgrp>
   <trd lng="id">metropolitan</trd>
   <trdgrp lng="it">
     <trd>megalopoli</trd>,
     <trd>metropoli</trd>
   </trdgrp>
   <trd lng="nl">grootstad</trd>
   <trd lng="ru">&c_m;&c_je;&c_g;&c_a;&c_p;&c_o;&c_l;&c_i;&c_s;</trd>
   <trd lng="sk">ve&lcaron;komesto</trd>
   <trd lng="uk">&c_v;&c_je;&c_l;&c_i;&c_ch;&c_je;&c_z;&c_n;&c_je; &c_m;&c_ib;&c_s;&c_t;&c_o;</trd>
  <trd lng="vo">lezif</trd>
</drv>

<drv mrk="urb.0estro">
  <kap><tld/>estro</kap>
  <snc mrk="urb.0estro.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Precipa administranto de <tld/>o:
      <ekz>
        la palacestro kaj la <tld/>estro kaj la pleja&gcirc;uloj kaj la filedukistoj sendis al
        <nom>Jehu</nom>
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 10:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        la re&gcirc;o <nom>Nebukadnecar</nom> sendis al la satrapoj, regionestroj, militestroj, &ccirc;efaj
        ju&gcirc;istoj, <tld/>estroj, le&gcirc;istoj, oficistoj, kaj potenculoj de la lando, ke
        ili venu
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 3:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili maltrankviligis la homamason kaj la <tld/>estrojn, kiuj tion a&ubreve;dis
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 17:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        en Damasko, la <tld/>estro sub la re&gcirc;o <nom>Aretas</nom> starigis gardistaron
        <fnt>
          <bib>NT</bib>
          <lok>II. Korintanoj 11:32</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi servis ja tie en la domo, kiam la gepatroj de la <tld/>estro ankora&ubreve; vivis
        kaj tie lo&gcirc;is
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>estro donis al li kvin spesmilojn por la voja&gcirc;o, kaj kelkaj urbanoj
