  </trdgrp>
  <trd lng="zh">&#x5E02;&#x6C11; <pr>sh&igrave; m&iacute;n</pr></trd>
</drv>

<drv mrk="urb.U0ano">
  <kap><tld lit="U"/>ano</kap>
  <snc mrk="urb.U0ano.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
	 la papoj <tld lit="U"/>ano VIII <klr>(la 8a)</klr> kaj Innocento
	 IV <klr>(la 4a)</klr>
	 <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj,
	 bagateloj</lok></fnt>;
      </ekz>
      <ekz>
        salutu <tld lit="U"/>anon, nian kunlaboranton por Kristo
        <fnt>
          <bib>NT</bib>
          <lok>Romanoj 16:9</lok>
        </fnt>;.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_U;&c_r;&c_b;&c_a;&c_n; <klr>(&c_ib;&c_m;&c_ja;)</klr></trd>
