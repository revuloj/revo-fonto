<drv mrk="urb.0ego">
   <kap><tld/>ego</kap>
   <snc mrk="urb.0ego.ARKI">
     <uzo tip="fak">ARKI</uzo>
     <dif>
       Tre granda <ref tip="super" cel="urb.0o.ARKI"><tld/></ref>okomplekso,
       kutime ampleksanta plurajn kunkreskintajn <tld/>ojn:
      <ekz>
        verdire la <tld/>ego <nom>Babel</nom> kun sia brua interpu&scirc;i&gcirc;ado kaj kun sia idolkulto ne
        allogas min
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        la luno <klr>[&#x2026;]</klr> &scirc;ajnis rigardi kun miro la pereon de la mondreganta
        <tld/>ego <klr>[&#x2026;]</klr>, Romo, kvaza&ubreve; grandega brul&scirc;tiparo, lumigis la tutan
        Kampanion
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi tuj iru al Stokholmo por intervjui nian agenton en tiu <tld/>ego
        <fnt>
          <bib>KKl</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> poste iris rekte al Berlino, kiun <tld/>egon ili atingis sen
        iaj notindaj spertoj
        <fnt>
          <bib>KKl</bib>
        </fnt>;
      </ekz>
      <ekz>
	 en la <tld/>egoj, &ccirc;iuj (neri&ccirc;uloj) lo&gcirc;as en
         anta&ubreve;<tld/>oj kaj &ccirc;iutage iras longan distancon buse a&ubreve; metroe
	 <fnt>
    	   <aut>B. Hekland</aut>,
    	   <vrk>Kompreni &ccirc;i-landon? Neniam!</vrk>,
    	   <bib>LOdE</bib>,
    	   <lok>1999:3, numero 53a</lok>
	 </fnt>;
      </ekz>
      <ekz>
	 la plej <ctl>delegitoza</ctl> <tld/>o plu restas la
	 brazila <tld/>ego <nom>Rio de Janeiro</nom>
	 <fnt>
	   <vrk>Preska&ubreve; du mil delegitoj</vrk>,
	   <bib>LOdE</bib>,
	   <lok>2000:8-9, numero 71a</lok>
	 </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
       <ref cel="aglome.0ajxo">aglomera&jcirc;o</ref>,
       <ref cel="metrop.0o.aglomerajxo">metropolo<sncref/></ref>,
       <ref cel="urb.milion0o">milionurbo</ref>
    </refgrp>
    <ref tip="vid" cel="civit.0o.urbo">civito<sncref/></ref>
  </snc>
   <trdgrp lng="be">
     <trd>&c_g;&c_o;&c_r;&c_a;&c_d; <klr>(&c_v;&c_ja;&c_l;&c_ib;&c_k;&c_ib;)</klr></trd>,
     <trd>&c_m;&c_e;&c_g;&c_a;&c_p;&c_o;&c_l;&c_ib;&c_s;</trd>
   </trdgrp>
   <trd lng="bg">&c_g;&c_o;&c_l;&c_ja;&c_m; <ind>&c_g;&c_r;&c_a;&c_d;</ind></trd>
   <trd lng="ca">metr&ograve;poli</trd>
   <trd lng="cs">velkom&ecaron;sto</trd>
   <trd lng="de">Gro&szlig;stadt</trd>
   <trd lng="en">megalopolis</trd>
   <trd lng="es">megal&oacute;polis</trd>
   <trd lng="fa">&f_k;&a_l;&a_A;&a_n;&#x200C;&a_sh;&a_h;&a_r;</trd>
   <trd lng="fi">suurkaupunki</trd>
   <trd lng="fr">m&eacute;galopole</trd>
   <trdgrp lng="hu">
     <trd>nagyv&aacute;ros</trd>,
     <trd>metropolisz</trd>
   </trdgrp>
   <trd lng="id">metropolitan</trd>
   <trdgrp lng="it">
     <trd>megalopoli</trd>,
     <trd>metropoli</trd>
   </trdgrp>
   <trd lng="ja">&#x5927;&#x90FD;&#x5E02;  <pr>&#x3060;&#x3044;&#x3068;&#x3057;</pr></trd>
  <trd lng="nl">grootstad</trd>
   <trd lng="ru">&c_m;&c_je;&c_g;&c_a;&c_p;&c_o;&c_l;&c_i;&c_s;</trd>
