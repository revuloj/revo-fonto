    <kap>
      <rad>Kampani</rad>/o
    </kap>

<drv mrk="kampani.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:regionoj" cel="region.0o.land">Regiono</ref> &ccirc;e la okcidenta 
      bordo de <ref tip="malprt" cel="ital.I0ujo">Italujo</ref> kun la &ccirc;efa urbo Napolo:
      <ekz>
        la&ubreve; la latina a&ubreve;toro Velejo Paterkulo, ili alvenis al <tld/>o sekvante la flugon
        de kolombo
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.net/2011/010261.php"
              >Estis iam alfabeto ...</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        terura tertremo &leftquot;ver&scirc;ajne&#8221; frapos ene de kelkaj monatoj la sudan Italion
        (<tld/>on, Kalabrion, Sicilion)
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.net/2013/010777.php"
              >&Gcirc;is la tero tremas</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Kampanien</trd>
  </snc>
</drv>

  </art>

