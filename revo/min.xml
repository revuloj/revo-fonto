<drv mrk="min.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="min.0i.elfosi">
    <uzo tip="fak">MIN</uzo>
    <dif>
      <ref tip="super" cel="eksplu.0i">Ekspluati</ref>, elfosi el <tld/>o:
      <ekz>
        kompanioj kiuj en Litovio <tld/>as petrolon
        <fnt>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2011/010416.php">Revoj
          pri skistogaso</url></vrk>
          <bib>Monato</bib>
          <lok>2011/08, p. 22</lok>
        </fnt>;
      </ekz>
      <ekz>
        jam 7000 jarojn tie oni <tld/>as salon
        <fnt>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2013/010970.php">Salutinda
          salminejo</url></vrk>
          <bib>Monato</bib>
          <lok>2013/07, p. 23</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_d;&c_a;&c_b;&c_y;&c_v;&c_a;&c_c;&c_mol;
      <klr>(&c_v;&c_y;&c_k;&c_a;&c_p;&c_n;&c_ib;)</klr></trd>,
      <trd>&c_r;&c_a;&c_s;&c_p;&c_r;&c_a;&c_c;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;
      <klr>(&c_r;&c_a;&c_d;&c_o;&c_v;&c_ib;&c_sh;&c_ch;&c_a;)</klr></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>abbauen</trd>,
      <trd>ausbeuten</trd>
    </trdgrp>
    <trd lng="es">minar</trd>
    <trd lng="pt">minerar</trd>
    <trdgrp lng="zh">
    <trd>&#21085;&#21066; <pr>b&#333;xu&emacron;</pr></trd>,
    <trd>&#21093;&#21066; <pr>b&#333;xu&emacron;</pr></trd>,
    <trd>&#21195;&#32489; <pr>x&#363;nj&#299;</pr></trd>,
    <trd>&#21235;&#32318; <pr>x&#363;nj&#299;</pr></trd>,
    <trd>&#26392; <pr>ju&amacron;n</pr></trd>,
    <trd>&#24320;&#37319; <pr>k&amacron;ic&#462;i</pr></trd>,
    <trd>&#38283;&#25505; <pr>k&amacron;ic&#462;i</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="min.0i.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Enmeti en foso a&ubreve; truo eksplodan substancon por renversi
      kaj detrui ion:
      <ekz>
        <tld/>i ponton;
      </ekz>
      <ekz>
        Pakistano, nervozigita, minacis bari kaj <tld/>i la linion
        <fnt>
          <bib>MD08</bib>
          <vrk>Inter naciismo kaj tuttera &gcirc;ihado</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="br">mina&ntilde;</trd>
    <trd lng="ca">minar</trd>
    <trdgrp lng="cs">
      <trd>dolovat</trd>,
      <trd>kutat</trd>
    </trdgrp>
    <trd lng="de">verminen</trd>
    <trd lng="en">mine</trd>
    <trd lng="es">minar</trd>
    <trd lng="fr">miner</trd>
    <trd lng="hu">al&aacute;akn&aacute;z</trd>
    <trd lng="it">minare</trd>
    <trd lng="nl">ondermijnen</trd>
    <trd lng="pl">zaminowywa&cacute;</trd>
    <trd lng="pt">minar</trd>
    <trd lng="ru">&c_m;&c_i;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    <trdgrp lng="sk">
      <trd>dolova&tcaron;</trd>,
      <trd>kuta&tcaron;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#20296;&#38647; <pr>b&ugrave;l&eacute;i</pr></trd>,
    <trd>&#24067;&#38647; <pr>b&ugrave;l&eacute;i</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="ja">
    <trd>&#22353;&#36947;&#12434;&#25496;&#12427;  <pr>&#12371;&#12358;&#12393;&#12358;&#12434;&#12411;&#12427;</pr></trd>,
    <trd>&#29190;&#34220;&#12434;&#20181;&#25499;&#12369;&#12427;
