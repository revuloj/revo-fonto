<drv mrk="min.0ado">
  <kap>
    <tld/>ado
  </kap>
  <snc>
    <dif>
      Ekspluatado de <ref cel="min.0o.MIN">mino<sncref/></ref>:
      <ekz>
        la metalo eniris la akvon pro kontra&ubreve;le&gcirc;a or-<tld/>ado
        <fnt>
          <bib>Monato</bib>
          <aut>Princo Henriko Oguinye/pg</aut>
          <vrk><url ref="&Monato;/2010/010224.php">Horor-minado</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la viroj okupi&gcirc;is pri <tld/>ado de la diverskolora granito el
        kiu konsistis la roko
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2009/009797.php">Pli pri
          la mistera insulo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Abbau <klr>(Erzmine)</klr></trd>
</drv>

   
<drv mrk="min.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Pli-malpli unuspeca minerala maso renkontata en
      <tld/>o<sncref ref="min.0o.MIN"/>:
      <ekz>
        <ind>sterila <tld/>a&jcirc;o</ind>;
        <trd lng="hu">medd&odblac;k&odblac;zet</trd>
        <trd lng="ru">&c_p;&c_u;&c_s;&c_t;&c_a;&c_ja; &c_p;&c_o;&c_r;&c_o;&c_d;&c_a;</trd>
      </ekz><ekz>
        or<tld/>a&jcirc;o, fer<tld/>a&jcirc;o;
      </ekz>
      <ekz>
        Ruando i&gcirc;is granda eksportisto de koltano, oro kaj diamantoj,
        Ugando de oro, diamanto kaj ligno <klr>[&#8230;]</klr> kiu finfine
        a&ccirc;etas la <tld/>a&jcirc;ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Serge Zandandu Ntomono Zola</aut>
          <vrk><url ref="https://www.monato.be/2003/007982.html">Disrabado
          de naturaj ri&ccirc;a&jcirc;oj</url></vrk>
        </fnt>?
      </ekz>
    </dif>
    <ref tip="sub" cel="erc.0o">erco</ref>
  <trdgrp lng="be">
    <trd>&c_g;&c_o;&c_r;&c_n;&c_a;&c_ja; &c_p;&c_a;&c_r;&c_o;&c_d;&c_a;</trd>,
    <trd>&c_r;&c_u;&c_d;&c_a;</trd>
  </trdgrp>                    
  <trd lng="br">kailh</trd>
  <trd lng="ca">mineral</trd>
  <trd lng="de">Erz</trd>
  <trd lng="en">ore</trd>
  <trd lng="es">veta</trd>
  <trdgrp lng="hu">
    <trd>b&aacute;nyk&odblac;zet</trd>,
    <trd>&eacute;rc</trd>
  </trdgrp>
  <trd lng="nl">erts</trd>
  <trdgrp lng="pl">
    <trd>kopalina</trd>,
    <trd>ruda</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_n;&c_a;&c_ja; &c_p;&c_o;&c_r;&c_o;&c_d;&c_a;</trd>
  </snc>
<trdgrp lng="de">
    <trd>Abbauprodukt</trd>,
    <trd>Rohstoff</trd>
  </trdgrp>
</drv>
    
<drv mrk="min.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Tuto de la aran&gcirc;a&jcirc;oj,
      <ref tip="prt" cel="sxakt.0o.mineja">&scirc;aktoj</ref>,
      <ref tip="prt" cel="galeri.0o.mineja">galerioj<sncref/></ref>
      a&ubreve;
      <ref tip="prt" cel="korido.0o.GEOL">koridoroj<sncref/></ref>
      kaj dependa&jcirc;oj faritaj por
      ekspluatado de <tld/>o,<sncref ref="min.0o.MIN"/>:
      <ekz>
        karbo<tld/>ejo, &scirc;ton<tld/>ejo;
      </ekz> 
      <ekz>
        en or<tld/>ejo oni uzas grandegajn ma&scirc;inojn por arigi
        etan kvanton da metalo, kiu fakte ne havas grandan utilon, 
        sed alte valoras, sole pro tio ke &gcirc;i estas malofta
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2013/011017.php">Bitcoin:
           &ccirc;u scienca eksperimento, trompado a&ubreve; reale uzebla virtuala
           mono?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en novembro pasintjare pereis 19 <tld/>istoj pro eksplodo &ccirc;e
        karbo<tld/>ejo
        <fnt>
          <bib>Monato</bib>
          <aut>D. E. Rogers</aut>
          <vrk><url ref="&Monato;/2011/010468.php">Tremoj
          teraj, financaj, politikaj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_a;&c_h;&c_t;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_p;&c_a;&c_l;&c_mol;&c_n;&c_ja;</trd>
  </trdgrp>
  <trd lng="br">min</trd>
  <trd lng="ca">mina</trd>
  <trd lng="cs">minej</trd>
  <trdgrp lng="de">
    <trd>Bergwerk</trd>,
    <trd>Grube <klr>(Bergwerk)</klr></trd>,
    <trd>Zeche <klr>(Bergwerk)</klr></trd>
  </trdgrp>
  <trd lng="en">mine</trd>
  <trd lng="es">mina</trd>
  <trd lng="fr">mine</trd>
  <trd lng="hu">b&aacute;nyatelep</trd>
  <trd lng="it">miniera</trd>
  <trd lng="nl">mijn</trd>
  <trd lng="pl">kopalnia</trd>
  <trdgrp lng="ru">
        <trd>&c_sh;&c_a;&c_h;&c_t;&c_a;</trd>,
        <trd>&c_k;&c_o;&c_p;&c_i;</trd></trdgrp>
  <trdgrp lng="sk">
    <trd>ba&ncaron;a</trd>,
    <trd>lom</trd>
  </trdgrp>
</drv>
    
<drv mrk="min.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="min.0i.elfosi">
    <uzo tip="fak">MIN</uzo>
    <dif>
      ekspluati, elfosi el <tld/>o:
      <ekz>
        kompanioj kiuj en Litovio <tld/>as petrolon
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2011/010416.php">Revoj
          pri skistogaso</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        jam 7000 jarojn tie oni <tld/>as salon
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2013/010970.php">Salutinda
          salminejo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>abbauen</trd>,
      <trd>ausbeuten</trd>
    </trdgrp>
  </snc>
  <snc mrk="min.0i.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Enmeti en foso a&ubreve; truo eksplodan substancon por renversi
      kaj detrui ion:
      <ekz>
        <tld/>i ponton.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="br">mina&ntilde;</trd>
    <trd lng="ca">minar</trd>
    <trdgrp lng="cs">
      <trd>dolovat</trd>,
      <trd>kutat</trd>
    </trdgrp>
    <trd lng="de">verminen</trd>
    <trd lng="en">mine</trd>
    <trd lng="fr">miner</trd>
    <trd lng="hu">al&aacute;akn&aacute;z</trd>
    <trd lng="it">minare</trd>
    <trd lng="nl">ondermijnen</trd>
    <trd lng="pl">zaminowywa&cacute;</trd>
    <trd lng="ru">&c_m;&c_i;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    <trdgrp lng="sk">
      <trd>dolova&tcaron;</trd>,
      <trd>kuta&tcaron;</trd>
    </trdgrp>
  </snc>
</drv>
    
    <drv mrk="min.kontraux0i">
      <kap>kontra&ubreve;<tld/>i</kap>
      <snc>
        <uzo tip="fak">MIL</uzo>
        <dif>
          Subfosi por malhelpi la <tld/>laboron de la malamiko.  
        </dif>
      </snc>
      <trd lng="be">&c_k;&c_o;&c_n;&c_t;&c_a;&c_r;&c_m;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
      <trd lng="ca">contraminar</trd>
      <trd lng="de">unterminieren</trd>
      <trdgrp lng="en">
        <trd>countermine</trd>,
        <trd>undermine</trd>,
