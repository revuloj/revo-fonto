<drv mrk="min.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Tuto de la aran&gcirc;a&jcirc;oj, sub&ccirc;ielaj fosoj a&ubreve; subteraj
      <ref tip="prt" cel="sxakt.0o.mineja">&scirc;aktoj</ref>,
