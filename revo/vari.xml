<drv mrk="vari.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="vari.0ajxo.KOMUNE">
    <dif>
      Kopio, <ref tip="super" cel="ekzemp1.0o">ekzemplero</ref>, okaza&jcirc;o diferencanta de
      originalo per iu eco.
    </dif>
    <subsnc mrk="vari.0ajxo.MUZ">
      <uzo tip="fak">MUZ</uzo>
      <dif>
        Interpreto <tld/>anta &ccirc;irka&ubreve; la &gcirc;eneralaj
        ritmo kaj melodio de originalo:
        <ekz>
          temo kun <tld/>a&jcirc;oj.
        </ekz>
      </dif>
      <refgrp tip="sin">
       <ref cel="variac.0o">variacio</ref>
      </refgrp>
    </subsnc>
    <subsnc mrk="vari.0ajxo.BIO">
      <uzo tip="fak">BIO</uzo>
      <dif>
        Individuo a&ubreve; grupo de individuoj diferencanta
        de la <ref tip="vid" cel="tip.0o.KOMUNE">tipo</ref>
        per iu eco:
        <ekz>
          la sangofago estas <tld/>a&jcirc;o de la specio fago
          <klr>(karakterizata de ru&gcirc;koloraj folioj)</klr>;
        </ekz><ekz>
          profesoro de botaniko <klr>[...]</klr> pririgardis la
          kreska&jcirc;on, gustumis &gcirc;in, sed <klr>[...]</klr> neniel
          povis trovi, al kiu klaso &gcirc;i apartenas; <ctl>tio estas
           <tld/>a&jcirc;o</ctl>, li diris
          <fnt><bib>Fab3</bib>, <lok>Historio de unu
          patrino</lok></fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="speci.sub0o.BIO">subspecio</ref>
      </refgrp>
    </subsnc>
    <refgrp tip="sub">
       <ref cel="varian.0o">varianto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_r;&c_y;&c_ja;&c_n;&c_t;</trd>,
    <trd>&c_v;&c_a;&c_r;&c_y;&c_ja;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
  <trd lng="bg">&c_v;&c_a;&c_r;&c_i;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Variation</trd>,
    <trd>Variet&auml;t</trd>.
  </trdgrp>
  <trdgrp lng="en">
    <trd>variant</trd>,
    <trd>variation</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>variante</trd>,
    <trd>variation<klr> (variante)</klr></trd>,
    <trd>vari&eacute;t&eacute;<klr> (variante)</klr></trd>,
  </trdgrp>
  <trdgrp lng="hu">
    <trd>v&aacute;ltozat</trd>,
    <trd>vari&aacute;ci&oacute;</trd>
    <trd>vari&aacute;ns</trd>
