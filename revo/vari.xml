<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>vari</rad>/i
</kap>
<drv mrk="vari.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="vari.0i.sxangxigxi">
    <dif>
      <ref tip="super" cel="sxangx.0igxi">&Scirc;an&gcirc;i&gcirc;adi</ref>
      la&ubreve;
      iu maniero sen da&ubreve;re perdi siajn esencajn trajtojn;
      &scirc;an&gcirc;i&gcirc;i la&ubreve; nur kelkaj ecoj, sed ne la&ubreve;
      aliaj:
      <ekz>
        la signifo de tiu vorto <tld/>as de lingvo al lingvo <klr>(la signifoj
        diferencas subtile, sed ne esence)</klr>;
      </ekz>
      <ekz>
        la kialoj <tld/>as de unu familio al alia, sed plej ofte temas
        pri ia kombino de la motivoj &ccirc;i-subaj
        <fnt>
        <aut>Jouko Lindstedt</aut>
        <vrk><url ref="http://www.helsinki.fi/~jslindst/odo.html">Oftaj
        demandoj pri denaskaj Esperant-lingvanoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ilia muzikstilo tre varias kaj ili &ccirc;iam bonege distris la homojn;
      </ekz>
      <ekz>
        la rakonta teniko <tld/>as, de klasika kronikado &ccirc;e Auld
        &gcirc;is vigla dialogo &ccirc;e Guglielminetti
        <fnt><aut>Sten JOHANSSON</aut>
        <vrk><url
        ref="http://www.dreamwater.net/esperanto/novel/novlibr/wiederborenrec.html">Bela
        Bukedo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="nl">
      <trd>vari&euml;ren</trd>,
      <trd>afwisselen</trd>
    </trdgrp>
  </snc>
  <snc mrk="vari.0i.devii">
    <dif>
      <ref tip="super" cel="devi.0i">Deviadi</ref> de avera&gcirc;o ene
      de certaj limoj; &scirc;an&gcirc;i&gcirc;i la&ubreve; la valoro,
      la&ubreve;grade:
      <ekz>
        informoj pri nombroj de parolantoj <tld/>as en diversaj fontoj
        <fnt>
        <aut>Bertilo Wennergren kaj Martin Weichert</aut>:
        <vrk><url ref="http://purl.oclc.org/net/lanlin/enkonduk.php">Landoj
        kaj Lingvoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la prezo en urbaj hoteloj <tld/>as de 50 &gcirc;is 150 e&ubreve;roj.
      </ekz>
    </dif>
    <trd lng="nl">werken<klr tip="ind"> (v.hout)</klr></trd>
  </snc>
  <refgrp tip="vid">
    <ref cel="sxangx">&scirc;an&gcirc;ema</ref>,
    <ref cel="konsta.0a">konstanta</ref>,
    <ref cel="fleksi.sen0a">senfleksia</ref>.
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_r;'&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_m;&c_ja;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_mol;&c_m;&c_je;&c_n;&c_mol;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_m;&c_je;&c_n;&c_mol;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_j;&c_n;&c_a;&c_ch;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_a;&c_z;&c_mol;&c_m;&c_je;&c_n;&c_mol;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de">variieren</trd>
  <trdgrp lng="es">
    <trd>variar<klr tip="ind"> (intr.)</klr></trd>,
    <trd>cambiar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>changer<klr tip="ind"> (intr.)</klr></trd>,
    <trd>varier</trd>
  </trdgrp>
  <trd lng="pt">variar</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_a;&c_r;&c_mol;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_i;&c_z;&c_m;&c_je;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="vari.0anta">
  <kap><tld/>anta</kap>
  <snc mrk="vari.0anta.KOMUNE">
    <dif>
      Kiu <tld/>as.
    </dif>
  </snc>
  <snc mrk="vari.0anta.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 8</lok></fnt>
    <dif>
      Libere elektebla ene de iu aro;
      estanta <ref cel="variab.0o">variablo</ref>:
      <ekz>
        lineara ekvacio kun <tld/>aj koeficientoj.
      </ekz>
    </dif>
    <ref tip="ant" cel="konsta.0a.gxenerale">konstanta.</ref>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_m;&c_je;&c_n;&c_n;&c_y;</trd>
  <trd lng="de">variabel</trd>
  <trd lng="en">variable</trd>
  <trd lng="es">variable</trd>
  <trd lng="fr">variable<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="vari.0anto">
  <kap><tld/>anto</kap>
  <uzo tip="fak">MAT</uzo>
  <uzo tip="stl">ARK</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 18</lok></fnt>
  <snc>
    <ref tip="dif" cel="variab.0o.MAT">variablo.</ref>
  </snc>
</drv>

<drv mrk="vari.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="kopi.0axjo">Kopio</ref> diferencanta de la
      <ref tip="vid" cel="origin1.0o">originalo</ref>
      en iu eco; individuo a&ubreve; grupo
      de individuoj diferencanta de la <ref tip="vid"
      cel="tip.0o.KOMUNE">tipo</ref>
      per iu eco:
      <ekz>
        <uzo tip="fak">MUZ</uzo>
        temo kun <tld/>a&jcirc;oj;
      </ekz>
      <ekz>
        <uzo tip="fak">BOT</uzo>
        la sangofago estas <tld/>a&jcirc;o de la specio fago
        <klr>(t.e. subspecio, karakterizata de ru&gcirc;koloraj folioj)</klr>;
      </ekz>
      <ekz>
        profesoro de botaniko <klr>[...]</klr> pririgardis la kreska&jcirc;on,
        gustumis &gcirc;in, sed <klr>[...]</klr> neniel povis trovi, al kiu
        klaso
        &gcirc;i apartenas; <ctl>tio estas <tld/>a&jcirc;o</ctl>, li diris
        <fnt><bib>Fab3</bib>, <lok>Historio de unu patrino</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Variation</trd>,
    <trd>Variet&auml;t</trd>.
  </trdgrp>
</drv>

<drv mrk="vari.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <dif>
      &Scirc;an&gcirc;i&gcirc;anta la&ubreve; la cirkonstancoj:
      <ekz>
        <tld/>ema vetero, vorto
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_mol;&c_m;&c_je;&c_n;&c_mol;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_m;&c_je;&c_n;&c_mol;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>voluble</trd>,
    <trd>variable</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>changeant</trd>,
    <trd>variable<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>inst&aacute;vel</trd>,
    <trd>mut&aacute;l</trd>,
    <trd>vari&aacute;vel</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_m;&c_je;&c_n;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_m;&c_je;&c_n;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
