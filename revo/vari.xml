<drv mrk="vari.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="vari.0i.sxangxigxi">
    <dif>
      <ref tip="super" cel="sxangx.0igxi">&Scirc;an&gcirc;i&gcirc;adi</ref>
      la&ubreve; iu maniero sen da&ubreve;re perdi siajn esencajn trajtojn;
      &scirc;an&gcirc;i&gcirc;i la&ubreve; nur kelkaj ecoj, sed ne la&ubreve; aliaj:
      <ekz>
        ilia muzikstilo tre <tld/>as kaj ili &ccirc;iam bonege distris
        la homojn;
      </ekz><ekz>
        la rakonta tekniko <tld/>as, de klasika kronikado &ccirc;e <nom>Auld</nom>
        &gcirc;is vigla dialogo &ccirc;e <nom>Guglielminetti</nom>
        <fnt><aut>S. Johansson</aut>
        <vrk><url ref="http://www.dreamwater.net/esperanto/novel/novlibr/wiederborenrec.html">
        Bela Bukedo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="nl">
      <trd>vari&euml;ren</trd>,
      <trd>afwisselen</trd>
    </trdgrp>
  </snc>
  <snc mrk="vari.0i.devii">
    <dif>
      <ref tip="super" cel="devi.0i">Deviadi</ref> de avera&gcirc;o ene
      de certaj limoj; &scirc;an&gcirc;i&gcirc;i la&ubreve; la valoro,
      la&ubreve;grade:
      <ekz>
        informoj pri nombroj de parolantoj <tld/>as en diversaj fontoj
        <fnt><bib>LanLin</bib></fnt>;
      </ekz><ekz>
        la prezo en urbaj hoteloj <tld/>as de 50 &gcirc;is 150 e&ubreve;roj.
      </ekz>
    </dif>
    <trd lng="nl">werken<klr tip="ind"> (v.hout)</klr></trd>
  </snc>
  <snc mrk="vari.0i.diversi">
    <dif>
      <ref tip="vid" cel="divers.0i.KOMUNE">Diversi</ref> ne esence,
      sed per etaj malsamoj:
      <ekz>
        la signifo de tiu vorto <tld/>as de lingvo al lingvo <klr>(la
        signifoj diferencas subtile, sed ne esence)</klr>;
      </ekz><ekz>
        la kialoj <tld/>as de unu familio al alia, sed plej ofte temas
        pri ia kombino de la motivoj &ccirc;i-subaj
        <fnt>
        <aut>J. Lindstedt</aut>
        <vrk><url ref="http://www.helsinki.fi/~jslindst/odo.html">Oftaj
        demandoj pri denaskaj Esperant-lingvanoj</url></vrk>
        </fnt>;
      </ekz><ekz>
        birdoj ene de la sama lando varias je rimarkinde malgranda grado
        <fnt>
          <bib>OriginoDeSpecioj</bib>,
          <lok>&ccirc;apitro 5a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
