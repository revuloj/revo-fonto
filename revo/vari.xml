<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>vari</rad>/i
</kap>
<drv mrk="vari.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Prezenti sinsekvajn diversajn &scirc;an&gcirc;i&gcirc;ojn:
      <ekz>
        la temperaturo sen&ccirc;ese <tld/>as hodia&ubreve;<fnt>B</fnt>;
      </ekz>
      <ekz>
        la moroj <tld/>as la&ubreve; tempoj kaj landoj;
      </ekz>
      <ekz>
        la salajro <tld/>as inter 4 kaj 12 dolaroj;
      </ekz>
      <ekz>
        dum horoj kaj horoj la pejza&gcirc;o ne <tld/>as;
      </ekz>
      <ekz>
        la &hcirc;ameleono <tld/>igas sian koloron la&ubreve; la lokoj, kiujn &gcirc;i
        trairas;
      </ekz>
      <ekz>
        <tld/>kolora kolibro<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="nl">
      <trd>vari&euml;ren</trd>,
      <trd>afwisselen</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Prezenti diversajn &scirc;an&gcirc;i&gcirc;ojn de volumeno, la&ubreve; la grado de malsekeco
      de la aero a&ubreve; de varmo p.p. ligno a&ubreve; metalo.
      <ekz>
        la <ind>ne<tld/>a</ind> irado de la astroj.
        <trdgrp lng="ru">
          <trd>&c_n;&c_je;&c_i;&c_z;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
          <trd>&c_i;&c_n;&c_v;&c_a;&c_r;&c_i;&c_a;&c_n;&c_t;&c_n;&c_y;&c_j;</trd>
        </trdgrp>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxangx">&scirc;angema</ref>,
      <ref cel="konsta.0a">konstanta</ref>,
      <ref cel="fleksi.sen0a">senfleksia</ref>.
    </refgrp>
    <trd lng="nl">werken<klr tip="ind"> (v.hout)</klr></trd>
  </snc>
  <trd lng="de">variieren</trd>
  <trdgrp lng="fr">
    <trd>changer<klr tip="ind"> (intr.)</klr></trd>,
    <trd>varier</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_a;&c_r;&c_mol;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_i;&c_z;&c_m;&c_je;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="vari.0a">
  <kap><tld/>a, <tld/>anta</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      Kvalifikas elementon libere elektebla ene de iu aro;
      estanta <ref cel="variab.0o">variablo</ref>:
      <ekz>
        lineara ekvacio kun <tld/>aj koeficientoj.
      </ekz>
    </dif>
    <ref tip="ant" cel="konsta.0a.gxenerale">konstanta.</ref>
  </snc>
  <trd lng="de">variable</trd>
  <trd lng="en">variable</trd>
  <trd lng="fr">variable<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="vari.0anto">
  <kap><tld/>anto</kap>
  <uzo tip="fak">MAT</uzo>
  <uzo tip="stl">ARK</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 18</lok></fnt>
  <snc>
    <ref tip="dif" cel="variab.0o.MAT">variablo.</ref>
  </snc>
</drv>

<drv mrk="vari.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <dif>
      &Scirc;an&gcirc;i&gcirc;anta la&ubreve; la cirkonstancoj:
      <ekz>
        <tld/>ema vetero, vorto
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>changeant</trd>,
    <trd>variable<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_m;&c_je;&c_n;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_m;&c_je;&c_n;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
