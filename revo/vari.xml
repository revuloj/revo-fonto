<drv mrk="vari.0a">
  <kap><tld/>a <fnt><bib>VdE</bib></fnt></kap>
  <snc mrk="vari.0a.sxangxigxema">
    <dif>
      &Scirc;an&gcirc;i&gcirc;anta la&ubreve; la cirkonstancoj; jen tia, jen alia:
      <ekz>
        <tld/>a vetero;
      </ekz><ekz>
        <tld/>a stelo;
      </ekz><ekz>
        La artikolo havas la formon <ctl>la</ctl>, <tld/>an nek
        la&ubreve;kaze, nek la&ubreve;genre, nek la&ubreve;nombre
        <fnt><vrk>PAG</vrk>, <lok>&para;76</lok></fnt>;
      </ekz><ekz>
        li penetris &gcirc;iskomprene la konfuzan menson kaj la <tld/>an
        naturon de virino
        <fnt>
          <aut>Cervantes, tr. Fernando de Diego</aut>:
          <vrk>La in&gcirc;enia hidalgo don Quijote de La Mancha</vrk>.
          <lok>Parto 1&#170;, &ccirc;ap. 27&#170;</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_je;&c_r;&c_a;&c_m;&c_je;&c_n;&c_n;&c_y;</trd>,
      <trd>&c_z;&c_mol;&c_m;&c_je;&c_n;&c_l;&c_ib;&c_v;&c_y;</trd>
    </trdgrp>
    <trd lng="en">variable<klr> (adj.)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_je;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_i;&c_z;&c_m;&c_je;&c_n;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="vari.0a.diversa">
    <dif>
      Prezentanta diversajn formojn, aspektojn, ecojn:
      <ekz>
        La programo de la festo estis tre <tld/>a
        <fnt><aut>Dimitar HA&Gcirc;IEV</aut>: <vrk>Festo de &gcirc;emeloj</vrk>.
             <bib>Monato</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_z;&c_n;&c_a;&c_s;&c_t;&c_a;&c_j;&c_n;&c_y;</trd>
    <trd lng="en">various</trd>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_n;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trd lng="bo">&t_ga;&t_yata;&t_shabchu;&t_ra;&t_tsheg;&t_ba;&t_tsheg;&t_cha;&t_drengbu;&t_na;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>
  <trdgrp lng="pl">
    <trd>urozmaicony</trd>,
    <trd>niesta&lstroke;y</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>divers</trd>,
    <trd>instabile</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_m;&c_ib;&c_n;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_m;&c_ib;&c_n;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_h;&c_i;&c_t;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_m;&c_ib;&c_n;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="vari.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="vari.0ajxo.KOMUNE">
    <dif>
      Kopio, <ref tip="super" cel="ekzemp1.0o">ekzemplero</ref>, okaza&jcirc;o diferencanta de
      originalo per iu eco.
    </dif>
    <subsnc mrk="vari.0ajxo.MUZ">
      <uzo tip="fak">MUZ</uzo>
      <dif>
        Interpreto <tld/>anta &ccirc;irka&ubreve; la &gcirc;eneralaj
        ritmo kaj melodio de originalo:
        <ekz>
          temo kun <tld/>a&jcirc;oj.
        </ekz>
      </dif>
      <refgrp tip="sin">
       <ref cel="variac.0o">variacio</ref>
      </refgrp>
    </subsnc>
    <subsnc mrk="vari.0ajxo.BIO">
      <uzo tip="fak">BIO</uzo>
      <dif>
        Individuo a&ubreve; grupo de individuoj diferencanta
        de la <ref tip="vid" cel="tip.0o.KOMUNE">tipo</ref>
        per iu eco:
        <ekz>
          la sangofago estas <tld/>a&jcirc;o de la specio fago
          <klr>(karakterizata de ru&gcirc;koloraj folioj)</klr>;
        </ekz><ekz>
          profesoro de botaniko <klr>[...]</klr> pririgardis la 
          kreska&jcirc;on, gustumis &gcirc;in, sed <klr>[...]</klr> neniel
          povis trovi, al kiu klaso &gcirc;i apartenas; <ctl>tio estas
           <tld/>a&jcirc;o</ctl>, li diris
          <fnt><bib>Fab3</bib>, <lok>Historio de unu
          patrino</lok></fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="speci.sub0o.BIO">subspecio</ref>
      </refgrp>
    </subsnc>
    <refgrp tip="sub">
       <ref cel="varian.0o">varianto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_r;&c_y;&c_ja;&c_n;&c_t;</trd>,
    <trd>&c_v;&c_a;&c_r;&c_y;&c_ja;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
  <trd lng="bg">&c_v;&c_a;&c_r;&c_i;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Variation</trd>,
    <trd>Variet&auml;t</trd>.
  </trdgrp>
  <trdgrp lng="en">
    <trd>variant</trd>,
    <trd>variation</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>variante</trd>,
    <trd>variation<klr> (variante)</klr></trd>,
    <trd>vari&eacute;t&eacute;<klr> (variante)</klr></trd>,
  </trdgrp>
  <trdgrp lng="hu">
    <trd>v&aacute;ltozat</trd>,
    <trd>vari&aacute;ci&oacute;</trd>
    <trd>vari&aacute;ns</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wariacja</trd>,
    <trd>wariant</trd>.
  </trdgrp>
  <trdgrp lng="ro">
    <trd>varia&#539;ie</trd>,
    <trd>varianta</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_a;&c_r;&c_i;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_v;&c_a;&c_r;&c_i;&c_a;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_m;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_h;&c_i;&c_l;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_v;&c_a;&c_r;&c_ib;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_v;&c_a;&c_r;&c_ib;&c_a;&c_c;&c_ib;&c_ja;</trd>,
    <trd>&c_b;&c_ib;&c_o;&c_l;. &c_v;&c_a;&c_r;&c_ib;&c_jeu;&c_t;&c_je;&c_t;</trd>,
    <trd>&c_r;&c_ib;&c_z;&c_n;&c_o;&c_v;&c_i;&c_d;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_v;&c_i;&c_d;</trd>,
