<drv mrk="vari.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vari.0igi.KOMUNE">
    <dif>
      <ref tip="vid" cel="modif.0i">Modifadi</ref>, ka&ubreve;zi <tld/>adon, 
      <ref tip="vid" cel="divers.0igi.KOMUNE">diversigi</ref>:
      <ekz>
        aparte en la provinco Guangxi mi komencis rimarki, ke iuj lokaj 
        legomoj agrable <tld/>igas la guston de la viandoj
        <fnt>
          <aut>P. Dasgupta</aut>,
          <vrk><url ref="http://esperanto.cri.cn/362/2008/12/04/1s89503.htm">
          Tagnoto...</url></vrk>,
          <lok>2008-12-04</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ja;&c_n;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol; <ind>&c_z;&c_mol;&c_m;&c_je;&c_n;&c_y;</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>m&ecaron;nit</trd>,
    <trd>zm&ecaron;nit</trd>,
    <trd>zpest&rcaron;it</trd>
  </trdgrp>
  <trd lng="en">cause to <ind>vary</ind></trd>
  <trd lng="fr">faire <ind>varier</ind></trd>
  <trdgrp lng="sk">
    <trd>meni&tcaron;</trd>,
    <trd>spestri&tcaron;</trd>,
    <trd>zmeni&tcaron;</trd>
  </trdgrp>
</drv>
</art>
