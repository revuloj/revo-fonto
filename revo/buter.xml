<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>buter</rad>/o
</kap>
<drv mrk="buter.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Grasa substanco eltirita el lakta kremo per trabatado:
      <ekz>
        vivi kiel kuko en <tld/>o
        <klr>(sate kaj glate)</klr><fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        &ccirc;io iris kiel sur <tld/>o
        <klr>(glate)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">m&aacute;slo</trd>
  <trd lng="de">Butter</trd>
  <trd lng="es">mantequilla</trd>
  <trd lng="en">butter</trd>
  <trd lng="fr">beurre</trd>
  <trd lng="he">&het;&mem;&alef;&he;</trd>
  <trd lng="hu">vaj</trd>
  <trd lng="nl">boter</trd>
  <trd lng="pl">mas&lstroke;o</trd>
  <trd lng="ru">&c_m;&c_a;&c_s;&c_l;&c_o; <klr>(&c_s;&c_l;&c_i;&c_v;&c_o;&c_ch;&c_n;&c_o;&c_je;)</klr></trd>
  <trd lng="sk">maslo</trd>
  <trd lng="sv">sm&ouml;r</trd>
  <trd lng="tr">tereya&gbreve;</trd>
</drv>

<drv mrk="buter.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      &Scirc;an&gcirc;i en <tld/>on.
    </dif>
  </snc>
  <trd lng="en">butter</trd>
  <trdgrp lng="fr">
    <trd>battre<klr tip="ind"> (en beurre)</klr></trd>,
    <trd>baratter</trd>
  </trdgrp>
  <trd lng="hu">vajat <ind>k&ouml;p&uuml;l</ind></trd>
  <trd lng="ru">&c_s;&c_b;&c_i;&c_v;&c_a;&c_t;&c_mol; &c_m;&c_a;&c_s;&c_l;&c_o;</trd>
</drv>

<drv mrk="buter.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <dif>
      Ujo por konservi kaj surtabligi la <tld/>on.
    </dif>
  </snc>
  <trd lng="de">Butterdose</trd>
  <trd lng="en">butter dish</trd>
  <trd lng="fr">beurrier</trd>
  <trd lng="hu">vajtart&oacute;</trd>
  <trd lng="ru">&c_m;&c_a;&c_s;&c_l;&c_jo;&c_n;&c_k;&c_a;</trd>
</drv>

<drv mrk="buter.0umi">
  <kap><tld/>umi</kap>
  <snc>
    <dif>
      &Scirc;miri <tld/>on sur pantran&ccirc;a&jcirc;on a&ubreve;
      en muldilon.
    </dif>
  </snc>
  <trd lng="en">butter</trd>
  <trd lng="fr">beurrer</trd>
  <trd lng="hu">vajat ken</trd>
  <trd lng="ru">&c_n;&c_a;&c_m;&c_a;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol; &c_m;&c_a;&c_s;&c_l;&c_o;&c_m;</trd>
  <trdgrp lng="sv">
    <trd>sm&ouml;ra</trd>,
    <trd>breda <klr>(p&aring; sm&ouml;r)</klr></trd>
  </trdgrp>
</drv>
</art>
