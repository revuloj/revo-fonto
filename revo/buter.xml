<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: buter.xml,v 1.35 2022/05/25 20:31:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>buter</rad>/o
</kap>
<drv mrk="buter.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Grasa substanco eltirita el lakta <ref cel="krem.0o.KUI">kremo</ref> per trabatado:
      <ekz>
        en belega kaliko &scirc;i alportis <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 5:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        batado de lakto produktas <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 30:33</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ricevadis pladon da ka&ccirc;o kun granda peco da <tld/>o en &gcirc;i
        <fnt>
          <bib>Fab3</bib>
          <lok>Koboldo &ccirc;e la butikisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;miri panon per <tld/>o
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>V</lok>
        </fnt>;
      </ekz>
      <ekz>
        plata kuko rostita en <tld/>o
        <fnt>
          <bib>LR</bib>
          <lok>Unua Parto &ndash; Leksikologio</lok>
        </fnt>;
      </ekz>
      <ekz>
        vivi kiel kuko en <tld/>o
        <klr>(sate kaj glate)
        </klr><fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        instruas mizero man&gcirc;i panon sen <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="buter.0o.simila">
    <dif>
      Simila <ref tip="super" cel="gras.0ajxo">grasa&jcirc;o</ref> eltirita el vegeta&jcirc;oj:
      <ekz>
        <tld/>o de kokosoj
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de arakidoj, de kakao.
      </ekz>
    </dif>
    <ref tip="vid" cel="margar.0o">margarino</ref>
  </snc>                  
  <trd lng="be">&c_m;&c_a;&c_s;&c_l;&c_a;</trd>
  <trd lng="bg">&c_m;&c_a;&c_s;&c_l;&c_o;</trd>
  <trd lng="bo">&t_ma;&t_ra;&t_tsheg;</trd>
  <trd lng="br">amanenn</trd>
  <trd lng="cs">m&aacute;slo</trd>
  <trd lng="de">Butter</trd>
  <trd lng="es">mantequilla</trd>
  <trd lng="en">butter</trd>
  <trd lng="fa">&f_k;&a_fatha;&a_r;&a_h;</trd>
  <trd lng="fr">beurre</trd>
  <trd lng="he">&het;&mem;&alef;&he;</trd>
  <trd lng="hu">vaj</trd>
  <trd lng="id">mentega</trd>
  <trd lng="nl">boter</trd>
  <trd lng="pl">mas&lstroke;o</trd>
  <trd lng="pt">manteiga</trd>
  <trd lng="ro">unt</trd>
  <trd lng="ru">&c_m;&c_a;&c_s;&c_l;&c_o;
  <klr>(&c_s;&c_l;&c_i;&c_v;&c_o;&c_ch;&c_n;&c_o;&c_je;)</klr></trd>
  <trd lng="sk">maslo</trd>
  <trd lng="sv">sm&ouml;r</trd>
  <trd lng="tr">tereya&gbreve;</trd>
  <trd lng="uk">&c_m;&c_a;&c_s;&c_l;&c_o;</trd>
</drv>

<drv mrk="buter.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      &Scirc;an&gcirc;i lakton en <tld/>on:
      <ekz>
        ma&scirc;ino por <tld/>igi
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#8470;8-9</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">ribotat</trd>
  <trd lng="cs">tlouci smetanu na m&aacute;slo</trd>
  <trd lng="de">Butter <ind>stampfen</ind></trd>
  <trd lng="en">butter</trd>
  <trdgrp lng="fr">
    <trd>battre<klr tip="ind"> (en beurre)</klr></trd>,
    <trd>baratter</trd>
  </trdgrp>
  <trd lng="hu">vajat <ind>k&ouml;p&uuml;l</ind></trd>
  <trd lng="pl">ubija&cacute; <ind>mas&lstroke;o</ind></trd>
  <trd lng="ru">&c_s;&c_b;&c_i;&c_v;&c_a;&c_t;&c_mol;
  &c_m;&c_a;&c_s;&c_l;&c_o;</trd>
  <trd lng="sk">m&uacute;ti&tcaron; maslo</trd>
</drv>

<drv mrk="buter.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <dif>
      Ujo por konservi kaj surtabligi la <tld/>on:
      <ekz>
        per tio vi ne &scirc;miros, tio estas froma&gcirc;o, kaj <tld/>o estas en la <tld/>ujo
        <fnt>
          <aut>J. German</aut>
          <vrk><url ref="http://www.esperanto.mv.ru/German/afero-11-2.html"
              >La afero, al kiu vi servas. 11.2</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">klozenn amanenn</trd>
  <trd lng="cs">schr&aacute;nka na m&aacute;slo</trd>
  <trd lng="de">Butterdose</trd>
  <trd lng="en">butter dish</trd>
  <trd lng="fa">&a_Z;&a_r;&a_f; &f_k;&a_r;&a_h;</trd>
  <trd lng="fr">beurrier</trd>
  <trd lng="hu">vajtart&oacute;</trd>
  <trd lng="pl">maselniczka</trd>
  <trd lng="ru">&c_m;&c_a;&c_s;&c_l;&c_jo;&c_n;&c_k;&c_a;</trd>
  <trd lng="sk">maselnica <klr>(na ulo&zcaron;enie masla)</klr></trd>
  <trd lng="uk">&c_n;&c_a;&c_s;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_c;&c_ja;</trd>
</drv>

<drv mrk="buter.0umi">
  <kap><tld/>umi</kap>
  <snc>
    <dif>
      <ref tip="super" cel="sxmir.0i.surmeti">&Scirc;miri</ref> <tld/>on sur 
      pantran&ccirc;a&jcirc;on a&ubreve; en muldilon:
      <ekz>
        varma <tld/>umita toasto
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 10 (60)</lok>
        </fnt>;
      </ekz>
      <ekz>
        farmista bruna pano, bone <tld/>umita per fre&scirc;a farmista natura <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Petro Desmet'</aut>
          <vrk><url ref="https://www.monato.be/2006/009004.php"
              >Jubile, jubilade, gargantuece palatkareseme</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_z;&c_a;&c_c;&c_mol;
  <ind>&c_m;&c_a;&c_s;&c_l;&c_a;&c_m;</ind></trd>
  <trd lng="br">amanenna&ntilde;</trd>
  <trd lng="cs">mazat m&aacute;slem</trd>
  <trd lng="de">buttern</trd>
  <trd lng="en">butter</trd>
  <trd lng="fr">beurrer</trd>
  <trd lng="hu">vajat ken</trd>
  <trd lng="pl">smarowa&cacute; <ind>mas&lstroke;em</ind></trd>
  <trd lng="ru">&c_n;&c_a;&c_m;&c_a;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;
  &c_m;&c_a;&c_s;&c_l;&c_o;&c_m;</trd>
  <trd lng="sk">natiera&tcaron; maslom</trd>
  <trdgrp lng="sv">
    <trd>sm&ouml;ra</trd>,
    <trd>breda <klr>(p&aring; sm&ouml;r)</klr></trd>
  </trdgrp>
</drv>
</art>
