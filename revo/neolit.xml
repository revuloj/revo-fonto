<vortaro>
<art mrk="$Id: neolit.xml,v 1.19 2021/03/04 20:49:19 revo Exp $">
<kap>
  <rad>neolitik</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="neolit.0o">
  <kap><tld/>o</kap>
  <snc mrk="neolit.0o.PRA">
    <uzo tip="fak">PRA</uzo>
    <dif>
      Prahistoria epoko, dum kiu oni uzadis ilojn el poluritaj &scirc;tonoj:
              <ekz>
        en tre frua tempo estis <tld/>a viro
             <fnt>
        <aut>Rudyard Kipling, Trad. Wouter F. Pilger</aut>,
        <vrk>Tiel do: Rakontoj por malgrandaj infanoj</vrk>,
        <lok><url ref=
        "http://www.gutenberg.org/ebooks/48900">
        gutenberg.org</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_e;&c_a;&c_l;&c_ib;&c_t;</trd>
  <trd lng="ca">neol&iacute;tic</trd>
  <trdgrp lng="cs">
    <trd>mlad&scaron;&iacute; doba kamenn&aacute;</trd>,
    <trd>neolit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Neolithikum</trd>,
    <trd>Jungsteinzeit</trd>
  </trdgrp>
  <trd lng="el">&ny;&epsilon;&omikron;&lambda;&jota;&theta;&jota;&kappa;&eta_ton; &epsilon;&pi;&omikron;&chi;&eta_ton;</trd>
  <trdgrp lng="en">
    <trd>Neolithic</trd>,
    <trd>Late Stone Age</trd>
  </trdgrp>
  <trd lng="es">neol&iacute;tico</trd>
  <trd lng="fa">&a_d;&a_w;&a_r;&a_h;&a_hamza_sure; <ind>&a_n;&a_w;&a_s;&a_n;&f_g;&f_y;</ind></trd>
  <trdgrp lng="fr">
    <trd>&acirc;ge de la pierre polie</trd>,
    <trd>n&eacute;olithique<klr tip="amb"> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>neolitikum</trd>,
    <trd>&uacute;jk&odblac;kor</trd>,
    <trd>csiszoltk&odblac;-kor</trd>
  </trdgrp>
  <trd lng="nl">Neolithicum</trd>
  <trd lng="pl">neolit</trd>
  <trd lng="pt">neol&iacute;tico</trd>
  <trd lng="ro">neolitic</trd>
  <trd lng="ru">&c_n;&c_je;&c_o;&c_l;&c_i;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>mlad&scaron;ia doba kamenn&aacute;</trd>,
    <trd>neolit</trd>
  </trdgrp>
</drv>
</art>
