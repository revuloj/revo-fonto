<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Nil</rad>/o
</kap>
<drv mrk="nil.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>ZR</bib></fnt>
  <fnt><bib>PIV1</bib></fnt>
  <fnt><bib>KrED</bib></fnt>
  <snc>
    <dif>
      Grandega nordafrika rivero ekfluanta en Etiopujo kaj Sudano kaj
      enfluanta en la <ref tip="vid"
      cel="mediterane.0o">Mediteraneon</ref> en
      Egiptujo. Apud <ref tip="vid" cel="amazon2.0o">Amazono</ref> la plej
      longa rivero en la mondo.
    </dif>
    <trd lng="be">&c_N;&c_ib;&c_l;</trd>
    <trd lng="de">Nil</trd>
    <trd lng="hu">N&iacute;lus</trd>
    <trd lng="lat">Nilus</trd>
    <trd lng="pt">Nilo</trd>
    <trd lng="ru">&c_N;&c_i;&c_l;</trd>
  </snc>
</drv>

<drv mrk="nil.blanka0o">
  <kap>Blanka <tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <fnt><bib>KrED</bib></fnt>
  <snc>
    <dif>
      Sudana alfluo al la Nilo.
    </dif>
    <trd lng="be">&c_B;&c_je;&c_l;&c_y; <ind>&c_N;&c_ib;&c_l;</ind></trd>
    <trd lng="de">Wei&szlig;er <ind>Nil</ind></trd>
    <trd lng="hu">Feh&eacute;r-N&iacute;lus</trd>
    <trd lng="pt">Nilo Branco</trd>
    <trd lng="ru">&c_B;&c_je;&c_l;&c_y;&c_j; <ind>&c_N;&c_i;&c_l;</ind></trd>
  </snc>
</drv>

<drv mrk="nil.blua0o">
  <kap>Blua <tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <fnt><bib>KrED</bib></fnt>
  <snc>
    <dif>
      Etiopuja alfluo al la Nilo.
    </dif>
    <trd lng="be">&c_B;&c_l;&c_a;&c_k;&c_ib;&c_t;&c_n;&c_y; <ind>&c_N;&c_ib;&c_l;</ind></trd>
    <trd lng="de">Blauer <ind>Nil</ind></trd>
    <trd lng="hu">K&eacute;k-N&iacute;lus</trd>
    <trd lng="pt">Nilo Azul</trd>
    <trd lng="ru">&c_G;&c_o;&c_l;&c_u;&c_b;&c_o;&c_j; <ind>&c_N;&c_i;&c_l;</ind></trd>
  </snc>
</drv>
</art>
