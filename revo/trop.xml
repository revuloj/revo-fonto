    <trd>troop</trd>,
    <trd>stijlfiguur</trd>
  </trdgrp>
  <trd lng="pt">tropo</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_o;&c_p;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_n;&c_o;&c_s;&c_n;&c_o;&c_je; &c_z;&c_n;&c_a;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
