<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>lir</rad>/o
</kap>
<drv mrk="lir.0o">
<kap><ofc>*</ofc><tld/>o</kap>
<snc num="1">
<uzo tip="fak">MUZ</uzo> 
<dif>
Antikva <ref tip="super" cel="instru2.kord0o">kordinstrumento</ref>. 
</dif>
<trd lng="en">lyre</trd>
<trd lng="nl">lier</trd>
</snc>
<snc num="2">
<dif>
Iama itala monero, entenanta 100 centimojn. 
</dif>
<trd lng="en">lira</trd>
<trd lng="nl">lira</trd>
</snc>
<trd lng="es">lira</trd>
</drv>
<drv mrk="lir.0ismo">
<kap><tld/>ismo</kap>
<snc>
<dif>
Poezia inspiro.  
</dif>
</snc>
<trd lng="nl">lyrisme</trd>
</drv>
</art>
