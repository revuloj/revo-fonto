<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lir</rad>/o
</kap>
<drv mrk="lir.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lir.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Iu el diversaj
      <ref tip="super" cel="instru2.kord0o">kordinstrumentoj</ref>
      kun sonkesto kiun ortas du stangoj supre kunigitaj
      per transversa stango, kaj kordoj fiksitaj
      inter la sonkesto kaj la transversa stango,
      kontraste al
      <ref tip="vid" cel="citr.0o.MUZ">citroj</ref>,
      <ref tip="vid" cel="harp.0o">harpoj</ref>,
      <ref tip="vid" cel="liut.0o">liutoj</ref>.
    </dif>
    <trdgrp lng="de">
      <trd>Leier</trd>,
      <trd>Lyra</trd>
    </trdgrp>
    <trd lng="en">lyre</trd>
    <trdgrp lng="fr">
      <trd>lyre</trd>
    </trdgrp>
    <trd lng="nl">lier</trd>
  </snc>
  <snc mrk="lir.0o.monunuo">
    <dif>
      Iama itala <ref tip="super" cel="unu.mon0o">monunuo</ref>,
      entenanta 100 centimojn.
    </dif>
    <trdgrp lng="de">
      <trd>Lire</trd>,
      <trd>Lira</trd>
    </trdgrp>
    <trd lng="en">lira</trd>
    <trdgrp lng="fr">
      <trd>lire</trd>
    </trdgrp>
    <trd lng="nl">lira</trd>
  </snc>
  <trd lng="es">lira</trd>
  <trd lng="ru">&c_l;&c_i;&c_r;&c_a;</trd>
</drv>

<drv mrk="lir.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      Poezia inspiro.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>lyrisme</trd>
  </trdgrp>
  <trd lng="nl">lyrisme</trd>
  <trd lng="ru">&c_l;&c_i;&c_r;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
