<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lir</rad>/o
</kap>
<drv mrk="lir.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Antikva <ref tip="super" cel="instru2.kord0o">kordinstrumento</ref>.
    </dif>
    <trd lng="en">lyre</trd>
    <trd lng="nl">lier</trd>
  </snc>
  <snc>
    <dif>
      Iama itala monero, entenanta 100 centimojn.
    </dif>
    <trd lng="en">lira</trd>
    <trd lng="nl">lira</trd>
  </snc>
  <trd lng="es">lira</trd>
  <trd lng="ru">&c_l;&c_i;&c_r;&c_a;</trd>
</drv>

<drv mrk="lir.0ismo">
  <kap>
    <tld/>ismo
  </kap>
  <snc>
    <dif>
      Poezia inspiro.
    </dif>
  </snc>
  <trd lng="nl">lyrisme</trd>
  <trd lng="ru">&c_l;&c_i;&c_r;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
