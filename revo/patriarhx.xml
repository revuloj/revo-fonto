<vortaro>
<art mrk="$Id$">
<kap>
  <rad>patriar&hcirc;</rad>/o
</kap>
<drv mrk="patriarhx.0o">
  <kap><tld/>o</kap>
  <snc>
    <ref tip="dif" cel="patria.0o">patriarko</ref>
    <ekz>
      per &ccirc;iuj sanktaj <tld/>oj!
      <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 14a, ruzo kontra&ubreve;
      ruzo</lok></fnt>;
    </ekz>
    <ekz>
      la sidejo de ilia <tld/>o estas en Bagdado
      <fnt><vrk>&Hcirc;aldea katolika Eklezio</vrk>
      <lok><url
      ref="http://eo.wikipedia.org/wiki/Crayon_Shin-chan">Vikipedio</url></lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_t;&c_r;&c_y;&c_ja;&c_r;&c_h;</trd>
  <trd lng="bg">&c_p;&c_a;&c_t;&c_r;&c_i;&c_a;&c_r;&c_h;</trd>
  <trd lng="ca">patriarca</trd>
  <trd lng="cs">patriarcha</trd>
  <trd lng="de">Patriarch</trd>
  <trd lng="en">patriarch</trd>
  <trd lng="es">patriarca</trd>
  <trd lng="fr">patriarche</trd>
  <trd lng="hu">p&aacute;tri&aacute;rka</trd>
  <trd lng="it">patriarca</trd>
  <trdgrp lng="nl">
    <trd>patriarch</trd>,
    <trd>aartsvader</trd>
  </trdgrp>
  <trd lng="pl">patriarcha</trd>
  <trd lng="pt">patriarca</trd>
  <trd lng="ru">&c_p;&c_a;&c_t;&c_r;&c_i;&c_a;&c_r;&c_h;</trd>
  <trd lng="sk">patriarcha</trd>
</drv>
</art>

