<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>ban</rad>/i
</kap>
<drv mrk="ban.0i">
   <kap><ofc>*</ofc><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc mrk="ban.0i.KOMUNE">
     <dif>
       Restigi iun a&ubreve; ion en akvo, flua&jcirc;o, precipe por lavi
       a&ubreve; kuraci:
       <ekz>
         <tld/>i infanon, &ccirc;evalon;
       </ekz>
       <ekz>
         li lavu siajn vestojn kaj <tld/>u sin en akvo
         <fnt><aut>&Z;</aut> <vrk>&X;</vrk><lok>&Lev; 15:5</lok></fnt>;
       </ekz>
       <klr>(f)</klr>
       <ekz>
         Anglujo estas &ccirc;iuflanke <tld/>ata de
         la maro.
       </ekz>
     </dif>
   </snc>
   <snc num="2">
     <uzo tip="klr">(f, p.p. likva&jcirc;o)</uzo>
     <dif>
       Disflui super, malsekigi:
       <ekz>
         larmoj <tld/>is liajn vangojn;
       </ekz>
       <ekz>
         &scirc;i estis <tld/>ata per larmoj;
       </ekz>
       <ekz>
         herbejoj <tld/>ataj de roso.
       </ekz>
     </dif>
   </snc>
   <snc mrk="ban.0i.TEK">
     <uzo tip="fak">TEK</uzo>
     <dif>
       Restigi ion en likva&jcirc;o por ricevi ian efikon:
       <ekz>
         <tld/>i fotografan paperon en rivelilo.
       </ekz>
     </dif>
   </snc>
   <refgrp tip="vid">
     <ref cel="satur.0i">saturi</ref>,
     <ref cel="tremp.0i">trempi</ref>,
     <ref cel="ond">&ccirc;irka&ubreve;ondi</ref>.
   </refgrp>
   <adm>
     La referenco al "&ccirc;irka&ubreve;ondi" estas stranga. (MB)
   </adm>
   <trd lng="cs">koupat</trd>
   <trd lng="de">baden</trd>
   <trd lng="fr">baigner</trd>
   <trd lng="nl">baden</trd>
   <trd lng="en">bathe</trd>
   <trd lng="ru">&c_k;&c_u;&c_p;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="ban.0igxi">
   <kap><tld/>i&gcirc;i</kap>
   <snc>
     <dif>
       Sin <tld/>i, fari&gcirc;i <tld/>ata:
       <ekz>
         la arboj <tld/>i&gcirc;as en la
         rivero;
       </ekz>
       <ekz>
         oni trovis lin <tld/>i&gcirc;antan en sia sango;
       </ekz>
       <ekz>
         vasta lando <tld/>i&gcirc;anta en la briloj de la suno
         <fnt><aut>&PtK;</aut>: <vrk>&Far1;</vrk>,<lok>&ccirc;ap.3</lok></fnt>.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="dron.0i">droni</ref>.
     </refgrp>
   </snc>
   <trd lng="fr">se <ind>baigner</ind></trd>
   <trd lng="ru">&c_k;&c_u;&c_p;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>
<drv mrk="ban.0o">
   <kap><tld/>o</kap>
   <snc num="1">
     <dif>
       Ago sin <tld/>i:
       <ekz>
         matene, post la <tld/>o, mi faras oferojn al la dio Amut
         <fnt><aut>&PtK;</aut>: 
<vrk>&Far1;</vrk>,<lok>&ccirc;ap.23</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <snc num="2">
     <dif>
       Flua&jcirc;o, en kiu oni <tld/>as
       <sncref ref="ban.0i.KOMUNE"/> a&ubreve;
       <sncref ref="ban.0i.TEK"/>:
       <ekz>
         mi preparas al vi <tld/>on kaj parfumojn
         <fnt><aut>&PtK;</aut>: <vrk>&Far1;</vrk>,<lok>&ccirc;ap.1</lok></fnt>;
       </ekz>
       <ekz>
         la <tld/>o estas tro varma.
       </ekz>
     </dif>
   </snc>
   <trd lng="de">Bad</trd>
   <trd lng="fr">bain</trd>
   <trd lng="nl">bad</trd>
   <trd lng="ru">&c_k;&c_u;&c_p;&c_a;&c_n;&c_i;&c_je;</trd>
</drv>
<drv mrk="ban.0ejo">
   <kap><tld/>ejo</kap>
   <snc>
     <dif>
       Domo, &ccirc;ambro, en kiu oni sin <tld/>as.
     </dif>
   </snc>
   <adm>
     Indus aldoni "~&ccirc;ambro" (sub "&ccirc;ambro")
     kaj referencon al &gcirc;i &ccirc;i tie. (MB)
   </adm>
   <trd lng="de">Bad</trd>
   <trdgrp lng="fr">
     <trd>bain (lieu)</trd>,
     <trd>salle de bain</trd></trdgrp>
   <trdgrp lng="nl"><trd>zwembad</trd>, <trd>bad</trd></trdgrp>
   <trdgrp lng="ru">
     <trd>&c_k;&c_u;&c_p;&c_a;&c_l;&c_mol;&c_n;&c_ja;</trd>,
     <trd>&c_b;&c_a;&c_n;&c_ja;</trd>
   </trdgrp>
</drv>
<drv mrk="ban.0ujo">
   <kap><tld/>ujo</kap>
   <snc>
     <dif>
       Bankuvo:
       <ekz>
         li sin <tld/>is en &scirc;tona <tld/>ujo
         <fnt><aut>&PtK;</aut>: <vrk>&Far1;</vrk>,<lok>&ccirc;ap.5</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="de">Wanne</trd>
   <trd lng="fr">baignoire</trd>
   <trd lng="nl">badkuip</trd>
   <trd lng="ru">&c_v;&c_a;&c_n;&c_n;&c_a;</trd>
</drv>
<drv mrk="ban.0urbo0loko">
   <kap><tld/>urbo, <tld/>loko</kap>
   <snc>
     <dif>
       Loko, kie oni kuracas per <tld/>oj (mineralaj, maraj).
     </dif>
   </snc>
   <trd lng="de">Badeort</trd>
   <trdgrp lng="fr">
     <trd>station thermale</trd>,
     <trd>ville d'eaux</trd></trdgrp>
   <trd lng="nl">badplaats</trd>
   <trd lng="ru">&c_k;&c_u;&c_r;&c_o;&c_r;&c_t;</trd>
</drv>
<drv mrk="ban.0gastoj">
   <kap><tld/>gastoj</kap>
   <snc>
     <dif>
       Malsanuloj, kiuj venas en <tld/>urbon por sin kuraci.
     </dif>
   </snc>
   <trd lng="de">Badeg&auml;ste</trd>
   <trd lng="fr">curistes (dans une station thermale)</trd>
   <trd lng="nl">badgasten</trd>
</drv>
<drv mrk="ban.fiks0o">
   <kap>fiks<tld/>o</kap>
   <uzo tip="fak">FOT</uzo>
   <snc>
     <dif>
       &Hcirc;emia solva&jcirc;o, uzata por igi fotografa&jcirc;on nedifektebla
       de la lumo.
     </dif>
   </snc>
   <trd lng="de">Fixierbad</trd>
   <trd lng="fr">fixateur</trd>
   <trdgrp lng="ru">
     <trd>&c_f;&c_i;&c_k;&c_s;&c_a;&c_zh;</trd>,
     <trd>&c_z;&c_a;&c_k;&c_r;&c_je;&c_p;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
   </trdgrp>
</drv>
<drv mrk="ban.sxvit0o">
   <kap>&scirc;vit<tld/>o</kap>
   <snc>
     <dif>
       <tld lit="B"/>o en seka tre varmigita aero.
     </dif>
   </snc>
   <trdgrp lng="de"><trd>Schwitzbad</trd>, <trd>Sauna</trd></trdgrp>
   <trdgrp lng="fr">
     <trd>sauna</trd>,
     <trd>bain de vapeur s&egrave;che</trd></trdgrp>
   <trd lng="nl">sauna</trd>
   <trd lng="ru">&c_s;&c_a;&c_u;&c_n;&c_a;</trd>
</drv>
<drv mrk="ban.sun0o">
   <kap>sun<tld/>o</kap>
   <snc>
     <dif>
       Elmetado de  la korpo al la sunradioj.
     </dif>
   </snc>
   <trd lng="de">Sonnenbad</trd>
   <trd lng="fr">bain de soleil</trd>
   <trd lng="ru">&c_s;&c_o;&c_l;&c_n;&c_je;&c_ch;&c_n;&c_a;&c_ja; 
&c_v;&c_a;&c_n;&c_n;&c_a;</trd>
</drv>

     <drv mrk="ban.vapor0o">
       <kap>vapor<tld/>o</kap>
       <snc>
         <dif>
           <tld lit="B"/>o en varma, tre malseka aero:
           <ekz>
             la varmego atingas 47 gradojn, kio egalas la temperaturon
             de nia vapor<tld/>o
             <fnt><aut>&PtK;</aut>: <vrk>&Far1;</vrk>,
             <lok>anta&ubreve;p.</lok></fnt>;
           </ekz>
           <ekz>
             du pastroj de malalta rango kondukis [lin] en vapor<tld/>ejon
             <fnt><aut>&PtK;</aut>: <vrk>&Far1;</vrk>,
             <lok>&ccirc;ap.20</lok></fnt>.
           </ekz>
         </dif>
       </snc>
       <trd lng="fr">bain de vapeur</trd>
     </drv>

</art>
