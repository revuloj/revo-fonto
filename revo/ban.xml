<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>ban</rad>/i
    </kap>

    <drv mrk="ban.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="ban.0i.KOMUNE">
        <dif>
          Restigi iun a&ubreve; ion en akvo, flua&jcirc;o, precipe por 
          lavi a&ubreve; kuraci:
          <ekz>
            <tld/>i infanon, &ccirc;evalon;
          </ekz><ekz>
            li lavu siajn vestojn kaj <tld/>u sin en akvo
            <fnt><bib>MT</bib>, <lok>&Lev; 15:5</lok></fnt>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            Anglujo estas &ccirc;iuflanke <tld/>ata de la maro.
          </ekz>
        </dif>
        <trd lng="br">souba&ntilde;</trd>
        <trdgrp lng="en">
          <trd>bathe</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>f&uuml;rdet</trd>,
          <trd>f&uuml;r&ouml;szt</trd>
        </trdgrp>
       <trdgrp lng="pl">
         <trd>k&aogonek;pa&cacute;</trd>,
         <trd>p&lstroke;awi&cacute;</trd>
       </trdgrp>
      </snc>
      <snc>
        <uzo tip="klr">(f, p.p. likva&jcirc;o)</uzo>
        <dif>
          Disflui super, malsekigi:
          <ekz>
            larmoj <tld/>is liajn vangojn;
          </ekz><ekz>
            &scirc;i estis <tld/>ata per larmoj;
          </ekz><ekz>
            herbejoj <tld/>ataj de roso.
          </ekz>
        </dif>
        <trdgrp lng="en">
          <trd>wash over</trd>,
          <trd>rinse</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>&aacute;ztat</trd>,
          <trd>f&uuml;r&ouml;szt</trd>
        </trdgrp>
        <trdgrp lng="pl">
          <trd>k&aogonek;pa&cacute;</trd>,
          <trd>oblewa&cacute;</trd>
        </trdgrp>
      </snc>
      <snc mrk="ban.0i.TEK">
        <uzo tip="fak">TEK</uzo>
        <dif>
          Restigi ion en likva&jcirc;o por ricevi ian efikon:
          <ekz>
            <tld/>i fotografan paperon en rivelilo.
          </ekz>
        </dif>
        <trd lng="br">souba&ntilde;</trd>
        <trdgrp lng="en">
          <trd>bathe</trd>,
          <trd>submerge</trd>,
          <trd>wash</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>&aacute;ztat</trd>,
          <trd>f&uuml;r&ouml;szt</trd>
        </trdgrp>
       <trdgrp lng="pl">
         <trd>k&aogonek;pa&cacute;</trd>
       </trdgrp>
      </snc>
      <refgrp tip="vid">
        <ref cel="satur.0i">saturi</ref>,
        <ref cel="tremp.0i">trempi</ref>,
        <ref cel="ond">&ccirc;irka&ubreve;ondi</ref>.
      </refgrp>
      <adm>
        La referenco al "&ccirc;irka&ubreve;ondi" estas stranga. (MB)
      </adm>
      <trd lng="be">&c_k;&c_u;&c_p;&c_a;&c_c;&c_mol;</trd>
      <trd lng="ca">banyar</trd>
      <trd lng="cs">koupat</trd>
      <trd lng="de">baden</trd>
      <trd lng="es">ba&ntilde;ar</trd>
      <trd lng="fr">baigner</trd>
      <trd lng="nl">baden</trd>
      <trd lng="pt">banhar</trd>
      <trdgrp lng="ru">
        <trd>&c_k;&c_u;&c_p;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_o;&c_k;&c_u;&c_n;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
      <trd lng="sk">k&uacute;pa&tcaron;</trd>
      <trd lng="vo">ban&ouml;n</trd>
    </drv>

    <drv mrk="ban.0igxi">
      <kap><tld/>i&gcirc;i</kap>
      <snc>
        <dif>
          Sin <tld/>i, fari&gcirc;i <tld/>ata:
          <ekz>
            la arboj <tld/>i&gcirc;as en la rivero;
          </ekz><ekz>
            oni trovis lin <tld/>i&gcirc;anta en sia sango;
          </ekz><ekz>
            vasta lando <tld/>i&gcirc;anta en la briloj de la suno
            <fnt><bib>Far1</bib>, <lok>&ccirc;ap.3</lok></fnt>.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="dron.0i">droni</ref>.
        </refgrp>
      </snc>
      <trd lng="be">&c_k;&c_u;&c_p;&c_a;&c_c;&c_c;&c_a;</trd>
      <trdgrp lng="br">
        <trd>kibella&ntilde;</trd>,
        <trd>kouronka&ntilde;</trd>
      </trdgrp>
      <trd lng="ca">banyar-se</trd>
      <trd lng="en">take a <ind>bath</ind></trd>
      <trd lng="es">ba&ntilde;arse</trd>
      <trd lng="fr">se <ind>baigner</ind></trd>
      <trdgrp lng="hu">
        <trd>f&uuml;rdik</trd>,
        <trd>&aacute;zik<klr tip="ind"> (folyad&eacute;kban)</klr></trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>k&aogonek;pa&cacute; si&eogonek;</trd>,
        <trd>p&lstroke;awi&cacute; si&eogonek;</trd>
      </trdgrp>
      <trd lng="ru">&c_k;&c_u;&c_p;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
      <trd lng="sk">k&uacute;pa&tcaron; sa</trd>
      <trd lng="kek">achink</trd>
    </drv>

    <drv mrk="ban.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Ago sin <tld/>i:
          <ekz>
            matene, post la <tld/>o, mi faras oferojn al la dio Amut
            <fnt><bib>Far1</bib>, <lok>&ccirc;ap.23</lok></fnt>.
          </ekz>
        </dif>
        <trdgrp lng="br">
          <trd>kibelladenn</trd>,
          <trd>kouronkadenn</trd>
        </trdgrp>
        <trd lng="hu">f&uuml;rd&eacute;s</trd>
        <trd lng="pl">k&aogonek;piel</trd>
      </snc>
      <snc>
        <dif>
          Flua&jcirc;o, en kiu oni <tld/>as
          <sncref ref="ban.0i.KOMUNE"/> a&ubreve;
          <sncref ref="ban.0i.TEK"/>:
          <ekz>
            mi preparas al vi <tld/>on kaj parfumojn
            <fnt><bib>Far1</bib>, <lok>&ccirc;ap.1</lok></fnt>;
          </ekz><ekz>
            la <tld/>o estas tro varma.
          </ekz>
        </dif>
        <trd lng="br">kibellad</trd>
        <trd lng="hu">f&uuml;rd&odblac;</trd>
      </snc>
  <trd lng="be">&c_k;&c_u;&c_p;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="ca">bany</trd>
  <trdgrp lng="cs">
    <trd>koupel</trd>,
    <trd>l&aacute;ze&ncaron;</trd>
  </trdgrp>
  <trd lng="de">Bad</trd>
  <trd lng="en">bath</trd>
  <trd lng="es">ba&ntilde;o</trd>
  <trd lng="fr">bain</trd>
  <trd lng="nl">bad</trd>
  <trd lng="oc">banh</trd>
  <trd lng="pl">k&aogonek;piel</trd>
  <trd lng="ro">baie</trd>
  <trd lng="ru">&c_k;&c_u;&c_p;&c_a;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">k&uacute;pe&lcaron;</trd>
  <trd lng="vo">ban</trd>
</drv>

<drv mrk="ban.0ejo">
      <kap><tld/>ejo</kap>
      <snc>
        <dif>
          Domo, &ccirc;ambro, en kiu oni sin <tld/>as.
        </dif>
      </snc>
      <adm>
        Indus aldoni "~&ccirc;ambro" (sub "&ccirc;ambro")
        kaj referencon al &gcirc;i &ccirc;i tie. (MB)
      </adm>
      <trdgrp lng="be">
        <trd>&c_k;&c_u;&c_p;&c_a;&c_l;&c_mol;&c_n;&c_ja;</trd>,
        <trd>&c_b;&c_a;&c_n;&c_ja;</trd>
      </trdgrp>
      <trd lng="bg">&c_b;&c_a;&c_n;&c_ja;</trd>
      <trdgrp lng="br">
        <trd>sal-gibella&ntilde;</trd>,
        <trd>kibelldi</trd>
      </trdgrp>
      <trdgrp lng="ca">
        <trd>piscina</trd>,
        <trd>balneari</trd>,
        <trd>lavabo</trd>
      </trdgrp>
      <trd lng="de">Bad</trd>
      <trd lng="en">bathroom</trd>
      <trdgrp lng="es">
        <trd>ba&ntilde;o<klr tip="ind"> (lugar)</klr></trd>,
        <trd>cuarto de ba&ntilde;o</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>bain<klr tip="ind"> (lieu)</klr></trd>,
        <trd>salle de bain</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>f&uuml;rd&odblac;szoba</trd>,
        <trd>f&uuml;rd&odblac;h&aacute;z</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>zwembad</trd>, <trd>bad</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>&lstroke;azienka</trd>,
        <trd>salon k&aogonek;pielowy</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_k;&c_u;&c_p;&c_a;&c_l;&c_mol;&c_n;&c_ja;</trd>,
        <trd>&c_b;&c_a;&c_n;&c_ja;</trd>
      </trdgrp>
      <trd lng="sk">k&uacute;pe&lcaron;&ncaron;a</trd>
      <trd lng="sk">k&uacute;pe&lcaron;&ncaron;a, k&uacute;pe&lcaron;n&yacute; dom</trd>
  <trd lng="sk">k&uacute;pe&lcaron;&ncaron;a, k&uacute;pe&lcaron;n&yacute; dom</trd>
  <trdgrp lng="cs">
    <trd>koupelna</trd>,
    <trd>l&aacute;ze&ncaron;</trd>
  </trdgrp>
</drv>

<drv mrk="ban.0isto">
      <kap><tld/>isto</kap>
      <snc>
        <dif>
          Servisto en <tld/>ejo, priservanta la <tld/>i&gcirc;antojn:
          <ekz>
            Vi devas agi tiel, ke <klr>(...)</klr> &ccirc;iuj <tld/>istoj,
            portistoj, skribistoj, oficiroj, veturigistoj
            <klr>(...)</klr> deziru la militon kontra&ubreve; Asirio
	    <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 25a.</lok></fnt>;
          </ekz><ekz>
            Dirinte tion Trasima&hcirc;o intencis foriri kvaza&ubreve; <tld/>isto
            ver&scirc;eginta vortojn sur niajn orelojn
            <fnt>
	      <aut>Platono, tr. D. Broadribb</aut>:
	      <vrk>La Respubliko</vrk>.
	      <lok>Parto 1a</lok>.
	    </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="en">bath attendant</trd>
      <trd lng="la">balneator</trd>
      <trd lng="ru">&c_b;&c_a;&c_n;&c_shch;&c_i;&c_k;</trd>
</drv>

<drv mrk="ban.0ujo">
      <kap><tld/>ujo</kap>
      <snc>
        <dif>
          Bankuvo:
          <ekz>
            li sin <tld/>is en &scirc;tona <tld/>ujo
            <fnt><bib>Far1</bib>, <lok>&ccirc;ap.5</lok></fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_v;&c_a;&c_n;&c_n;&c_a;</trd>
  <trd lng="bg">&c_v;&c_a;&c_n;&c_a;</trd>
  <trd lng="br">kibell</trd>
  <trd lng="ca">banyera</trd>
  <trd lng="cs">l&aacute;ze&ncaron;</trd>
  <trdgrp lng="de">
        <trd>Badewanne</trd>,
        <trd>Wanne</trd>
      </trdgrp>
  <trd lng="en">bathtub</trd>
  <trd lng="es">ba&ntilde;era</trd>
  <trd lng="fr">baignoire</trd>
  <trd lng="hu">f&uuml;rd&odblac;k&aacute;d</trd>
  <trd lng="nl">badkuip</trd>
  <trdgrp lng="pl">
        <trd>wanna</trd>,
        <trd>niecka k&aogonek;pielowa</trd>
      </trdgrp>
  <trd lng="ru">&c_v;&c_a;&c_n;&c_n;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>k&uacute;pe&lcaron;</trd>,
    <trd>va&ncaron;a</trd>
  </trdgrp>
</drv>

<drv mrk="ban.bol0o">
  <kap>bol<tld/>o</kap>
    <snc mrk="ban.bol0o.KUI">
    <uzo tip="fak">KEM</uzo>
    <uzo tip="fak">KUI</uzo>
    <dif>
      Speciala varmigo kiu konsistas el mergado de vazo en
      varmegan a&ubreve; bolantan likvon.
      <ekz>
        finfine, en la bakujo varmigita je 180&deg; C., dum
        30 minutoj oni bol<tld/>as plurajn individuajn 
        pladetojn entenantajn la lakta&jcirc;on kiu fari&gcirc;os fla&ubreve;no.
        <fnt>
          <aut>M. Corobu</aut>,
          <vrk><url
          ref="http://groups.google.com.pe/group/soc.culture.esperanto/msg/ee57339931d8bfd4">
            soc.culture.esperanto, 2010-09-24</url></vrk>      
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca"><ind>bany</ind> maria</trd>
  <trd lng="de">Wasserbad</trd>
  <trd lng="fr">bain-marie</trd>
  <trd lng="it">bagnomaria</trd>
  <trd lng="ru">&c_v;&c_o;&c_d;&c_ja;&c_n;&c_a;&c_ja; &c_b;&c_a;&c_n;&c_ja;</trd>
</drv>

<drv mrk="ban.fiks0o">
      <kap>fiks<tld/>o</kap>
      <uzo tip="fak">FOT</uzo>
      <snc>
        <dif>
          &Hcirc;emia solva&jcirc;o, uzata por igi fotografa&jcirc;on 
          nedifektebla de la lumo.
        </dif>
      </snc>
  <trd lng="be">&c_f;&c_ib;&c_k;&c_s;&c_a;&c_zh;</trd>
  <trd lng="br">stabilaer<klr tip="ind"> (luc'hskeudennerezh)</klr>
      </trd>
  <trd lng="ca">fixador</trd>
  <trd lng="cs">ustalovac&iacute; l&aacute;ze&ncaron;</trd>
  <trd lng="de">Fixierbad</trd>
  <trd lng="en">fixing agent</trd>
  <trd lng="fr">fixateur</trd>
  <trdgrp lng="hu">
        <trd>r&ouml;gz&iacute;t&odblac;f&uuml;rd&odblac;</trd>,
        <trd>fix&aacute;l&odblac;f&uuml;rd&odblac;</trd>
      </trdgrp>
  <trd lng="nl">fixeerbad</trd>
  <trd lng="pl">utrwalacz</trd>
  <trdgrp lng="ru">
        <trd>&c_f;&c_i;&c_k;&c_s;&c_a;&c_zh;</trd>,
        <trd>&c_z;&c_a;&c_k;&c_r;&c_je;&c_p;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
      </trdgrp>
  <trd lng="sk">usta&lcaron;ovac&iacute; k&uacute;pe&lcaron;</trd>
</drv>

    <drv mrk="ban.sun0o">
      <kap>sun<tld/>o</kap>
      <snc>
        <dif>
          Elmetado de la korpo al la sunradioj.
        </dif>
      </snc>
  <trd lng="be">&c_s;&c_o;&c_n;&c_je;&c_ch;&c_n;&c_a;&c_ja; &c_v;&c_a;&c_n;&c_n;&c_a;</trd>
  <trd lng="br">heoliadenn</trd>
  <trd lng="ca"><ind>bany</ind> de sol</trd>
  <trdgrp lng="cs">
    <trd>opalov&aacute;n&iacute; se</trd>,
    <trd>slune&ccaron;n&iacute; l&aacute;ze&ncaron;</trd>
  </trdgrp>
  <trd lng="de">Sonnenbad</trd>
  <trd lng="en">sunbath</trd>
  <trd lng="fr">bain de soleil</trd>
  <trd lng="hu">napf&uuml;rd&odblac;</trd>
  <trd lng="nl">zonnebad</trd>
  <trd lng="pl">k&aogonek;piel s&lstroke;oneczna</trd>
  <trd lng="ru">&c_s;&c_o;&c_l;&c_n;&c_je;&c_ch;&c_n;&c_a;&c_ja; 
      &c_v;&c_a;&c_n;&c_n;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>opa&lcaron;ovanie</trd>,
    <trd>slne&ccaron;n&yacute; k&uacute;pe&lcaron;</trd>
  </trdgrp>
</drv>

    <drv mrk="ban.sxvit0o">
      <kap>&scirc;vit<tld/>o</kap>
      <snc>
        <dif>
          <tld lit="B"/>o en seka tre varmigita aero.
        </dif>
        <refgrp tip="vid">
          <ref cel="sauxn.0o">sa&ubreve;no</ref>
        </refgrp>
      </snc>
      <adm>
        Kial &ccirc;i tio nomi&gcirc;as "bano"?  Temas pri &scirc;vitado
        en aero, ne banado en likvo!  Krome, la Fundamenta 
        "&scirc;vitbanejo" rilatas al vaporbanejo, ne al sa&ubreve;no 
        (kiu lasta ankora&ubreve; ne ekzistis).
        <aut>Sergio</aut>
      </adm>
      <trd lng="be">&c_s;&c_a;&c_w;&c_n;&c_a;</trd>
      <trd lng="br">saona</trd>
      <trd lng="ca">sauna</trd>
      <trdgrp lng="de">
        <trd>Schwitzbad</trd>, <trd>Sauna</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>sauna</trd>,
        <trd>bain de vapeur s&egrave;che</trd>
      </trdgrp>
      <trd lng="en">sauna</trd>
      <trdgrp lng="hu">
        <trd>izzaszt&oacute;f&uuml;rd&odblac;</trd>,
        <trd>szauna</trd>
      </trdgrp>
      <trd lng="nl">sauna</trd>
      <trdgrp lng="pl">
        <trd>&lstroke;a&zacute;nia parowa</trd>,
        <trd>sauna</trd>,
        <trd>par&oacute;wka</trd>
      </trdgrp>
      <trd lng="ru">&c_s;&c_a;&c_u;&c_n;&c_a;</trd>
    </drv>

    <drv mrk="ban.sxvit0ejo">
      <kap><ofc>*</ofc>&scirc;vit<tld/>ejo</kap>
      <snc>
        <dif>
          Parto de <tld/>ejo en kiu oni sin &scirc;vitigas per
          varmega vaporo:
          <ekz>
            la kuracisto konsilis al mi iri en &scirc;vit<tld/>ejon
            <fnt><bib>F</bib>, <lok>&FE;40.</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <rim>
        Kiel evidentas el la ekzemplo, la Fundamenta
        vorto &scirc;vit<tld/>ejo aperis jam anta&ubreve; la apero de 
        sa&ubreve;noj.
      </rim>
      <trd lng="be">&c_p;&c_a;&c_r;&c_y;&c_l;&c_k;&c_a;</trd>
      <trd lng="ca">sauna</trd>
      <trdgrp lng="pl">
        <trd>&lstroke;a&zacute;nia parowa</trd>,
        <trd>sauna</trd>,
        <trd>par&oacute;wka</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_p;&c_a;&c_r;&c_i;&c_l;&c_k;&c_a;</trd>,
        <trd>&c_p;&c_a;&c_r;&c_n;&c_a;&c_ja; &c_b;&c_a;&c_n;&c_ja;</trd>
      </trdgrp>
    </drv>

    <drv mrk="ban.vapor0o">
      <kap>vapor<tld/>o</kap>
      <snc>
        <dif>
          <tld lit="B"/>o en varma, tre malseka aero:
          <ekz>
            la varmego atingas 47 gradojn, kio egalas la temperaturon
            de nia vapor<tld/>o
            <fnt><bib>Far1</bib>, <lok>anta&ubreve;p.</lok></fnt>;
          </ekz><ekz>
            du pastroj de malalta rango kondukis [lin] en 
            vapor<tld/>ejon <fnt><bib>Far1</bib>, <lok>&ccirc;ap.20
            </lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <adm>
        Kial &ccirc;i tio nomi&gcirc;as "bano"?  Temas pri &scirc;vitado
        en vaporo, ne banado en likvo!  Devas temi pri "vaporbanejo", 
        t.e. banejo en kiu estas aparta ejo por vapor&scirc;vitado.
        <aut>Sergio</aut>
      </adm>
      <trd lng="be">&c_p;&c_a;&c_r;&c_y;&c_l;&c_k;&c_a;</trd>
      <trdgrp lng="ca">
        <trd><ind>bany</ind> turc</trd>,
        <trd><ind>bany</ind> de vapor</trd>
      </trdgrp>
      <trd lng="de">Dampfbad</trd>
      <trd lng="en">steambath</trd>
      <trd lng="fr">bain de vapeur</trd>
      <trd lng="hu">g&odblac;z&uuml;rd&odblac;</trd>
      <trd lng="nl">stoombad</trd>
      <trd lng="ru">&c_p;&c_a;&c_r;&c_i;&c_l;&c_k;&c_a;</trd>
      <trdgrp lng="pl">
        <trd>&lstroke;a&zacute;nia parowa</trd>,
        <trd>par&oacute;wka</trd>
      </trdgrp>
      <trd lng="sk">parn&yacute; <ind>k&uacute;pe&lcaron;</ind></trd>
      <trd lng="sk">parn&yacute; k&uacute;pe&lcaron;</trd>
  <trd lng="cs">parn&iacute; l&aacute;ze&ncaron;</trd>
</drv>

    <drv mrk="ban.0gastoj">
      <kap><tld/>gastoj</kap>
      <snc mrk="ban.0gastoj.MED">
        <uzo tip="fak">MED</uzo>
        <dif>
          Malsanuloj, kiuj venas en <tld/>urbon por sin kuraci.
        </dif>
      </snc>
      <trd lng="ca">banyistes de balneari</trd>
      <trd lng="de">Badeg&auml;ste</trd>
      <trd lng="en">bathers</trd>
      <trd lng="fr">curistes<klr tip="ind"> (dans une station
          thermale)</klr></trd>
      <trd lng="hu">f&uuml;rd&odblac;vend&eacute;gek</trd>
      <trd lng="nl">badgasten</trd>
      <trd lng="pl">kuracjusze</trd>
      <trd lng="ru">&c_k;&c_u;&c_r;&c_o;&c_r;&c_t;&c_n;&c_i;&c_k;&c_i;
      </trd>
    </drv>

    <drv mrk="ban.0urbo0loko">
      <kap><tld/>urbo, <var><kap><tld/>loko</kap></var></kap>
      <snc mrk="ban.0urbo0loko.MED">
        <uzo tip="fak">MED</uzo>
        <dif>
          Loko, kie oni kuracas per <tld/>oj (mineralaj, maraj).
        </dif>
      </snc>
  <trd lng="be">&c_k;&c_u;&c_r;&c_o;&c_r;&c_t;</trd>
  <trdgrp lng="ca">
        <trd>balneari</trd>,
        <trd>ciutat balneari</trd>
      </trdgrp>
  <trd lng="cs">l&aacute;zn&ecaron; - skupina budov</trd>
  <trd lng="de">Badeort</trd>
  <trd lng="en"><klr>(hot) </klr>springs</trd>
  <trd lng="es">balneario</trd>
  <trdgrp lng="fr">
        <trd>station thermale</trd>,
        <trd>ville d'eaux</trd>
      </trdgrp>
  <trd lng="hu">f&uuml;rd&odblac;hely</trd>
  <trd lng="nl">badplaats</trd>
  <trdgrp lng="pl">
        <trd>uzdrowisko</trd>,
        <trd>kurort</trd>
      </trdgrp>
  <trd lng="ru">&c_k;&c_u;&c_r;&c_o;&c_r;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>k&uacute;pele</trd>,
