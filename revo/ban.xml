  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_a;&c_r;&c_i;&c_l;&c_mol;&c_n;&c_ja;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_n;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_n;&c_a; &c_l;&c_a;&c_z;&c_n;&c_ja;</trd>,
    <trd>&c_t;&c_je;&c_r;&c_m;&c_i;</trd>,
    <trd>&c_r;&c_i;&c_m;&c_s;&c_mol;&c_k;&c_a; &c_l;&c_a;&c_z;&c_n;&c_ja;</trd>,
    <trd>&c_s;&c_a;&c_u;&c_n;&c_a;</trd>
