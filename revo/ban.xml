</kap>

<drv mrk="ban.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ban.0i.KOMUNE">
    <dif>
      Restigi iun a&ubreve; ion en akvo, flua&jcirc;o, precipe por lavi a&ubreve; kuraci:
      <ekz>
        virinoj <tld/>is la etulon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        li lavu siajn vestojn kaj <tld/>u sin en akvo, kaj vespere li estos pura
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 19:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        li rapide sin <tld/>is kaj oleis, surmetis novan tola&jcirc;on
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>as sin en lakto de azeninoj
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i sin en la rivero
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        se la akvo jam frosti&gcirc;is, oni faras truon en la glacion kaj sin <tld/>as
        <fnt>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2009/009629.php">Vintra banado</url></vrk>
           <bib>Monato</bib>
          <lok>2009/01, p. 15</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> devas partopreni en &ccirc;iutagaj kura&gcirc;pruvoj, kiel man&gcirc;i
        araneegojn, <tld/>i <klr>[sin]</klr> en &scirc;limo
        <fnt>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2005/008601.php">Torturado televida</url></vrk>
          <bib>Monato</bib>
          <lok>2005/06, p. 13</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la suno fluas en la brilanta torento <klr>[&#8230;]</klr>
        sin <tld/>as en fluetoj de
        l&#8217; fontanoj sur grandaj placoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="br">souba&ntilde;</trd>
    <trdgrp lng="en">
      <trd>bathe</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>f&uuml;rdet</trd>,
      <trd>f&uuml;r&ouml;szt</trd>
    </trdgrp>
   <trdgrp lng="id">
     <trd>mandi</trd>,
     <trd><baz>mandi</baz>memandikan</trd>
   </trdgrp>
   <trdgrp lng="pl">
     <trd>k&aogonek;pa&cacute;</trd>,
     <trd>p&lstroke;awi&cacute;</trd>
   </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <uzo tip="klr">(p.p. likva&jcirc;o)</uzo>
    <dif>
      Disflui super, malsekigi:
      <ekz>
        gutoj de &scirc;vito <tld/>is lian frunton
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>;
      </ekz>
      <ekz>
        subite etendi&gcirc;is anta&ubreve; mi la grandioza spektaklo de interna mondo: vasta funga
        riverbordo <klr>[&#8230;]</klr> <tld/>ata de lar&gcirc;a olea rivero, kiu fluis el
        abismoj teruraj kaj nekonataj
        <fnt>
          <bib>Sol</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>wash over</trd>,
      <trd>rinse</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&aacute;ztat</trd>,
      <trd>f&uuml;r&ouml;szt</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd><baz>basah</baz>membasahi</trd>,
      <trd><baz>basuh</baz>membasuh</trd>,
      <trd><baz>bilas</baz>membilas</trd>,
      <trd><baz>cuci</baz>mencuci</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>k&aogonek;pa&cacute;</trd>,
      <trd>oblewa&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="ban.0i.en_abundo">
    <uzo tip="klr">(sin, pasive)</uzo>
    <dif>
      <ref cel="gxu.0i.posedi">&Gcirc;ui</ref> abundon da;
      <ref cel="nagx.0i.FIG_enesti">na&gcirc;i<sncref/></ref>:
      <ekz>
        <tld/>i sin en lukso;
      </ekz>
      <ekz>
        <tld/>ataj en lazuro de feli&ccirc;igaj pensoj
        <fnt>
          <bib>ChV</bib>
          <lok>18</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i staris &ccirc;e la jam malfermita fenestro kaj
        <tld/>is sian viza&gcirc;on en fre&scirc;a aero
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        se la elektro ne estus tiel multekosta, mi <tld/>us min en lumo
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>.
      </ekz>
    </dif>
  <trdgrp lng="id">
    <trd><baz>mandi</baz>bermandi</trd>,
    <trd><baz>mandi</baz>bermandikan</trd>
  </trdgrp>
  </snc>
  <snc mrk="ban.0i.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Restigi ion en likva&jcirc;o por ricevi ian efikon:
      <ekz>
        <tld/>i fotografan paperon en rivelilo.
      </ekz>
    </dif>
    <trd lng="br">souba&ntilde;</trd>
    <trdgrp lng="en">
      <trd>bathe</trd>,
      <trd>submerge</trd>,
      <trd>wash</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&aacute;ztat</trd>,
      <trd>f&uuml;r&ouml;szt</trd>
    </trdgrp>
   <trdgrp lng="id">
     <trd><baz>cuci</baz>mencuci foto</trd>,
     <trd><baz>rendam</baz>merendam<klr> (foto)</klr></trd>
   </trdgrp>
   <trdgrp lng="pl">
     <trd>k&aogonek;pa&cacute;</trd>
   </trdgrp>
  </snc>
  <refgrp tip="vid">
    <ref cel="satur.0i">saturi</ref>,
    <ref cel="tremp.0i">trempi</ref>,
    <ref cel="ond">&ccirc;irka&ubreve;ondi</ref>
  </refgrp>
  <adm>
    La referenco al "&ccirc;irka&ubreve;ondi" estas stranga. (MB)
  </adm>
  <trd lng="be">&c_k;&c_u;&c_p;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">banyar</trd>
  <trd lng="cs">koupat</trd>
  <trd lng="de">baden <klr>(jmd.)</klr></trd>
  <trd lng="es">ba&ntilde;ar</trd>
  <trd lng="fr">baigner</trd>
  <trd lng="he">&lamed;&resh;&het;&vav;&ftsadi;</trd>
  <trdgrp lng="ja">
    <trd>&#x6C34;&#x6D74;&#12373;&#x305B;&#x308B;  <pr>&#12377;&#12356;&#12424;&#x304F;&#12373;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x6D78;&#12375;&#12390;&#x304A;&#x304F;  <pr>&#12402;&#x305F;&#12375;&#12390;&#x304A;&#x304F;</pr></trd>,
    <trd>&#x6D74;&#12403;&#x305B;&#x304B;&#12369;&#x308B;  <pr>&#12354;&#12403;&#x305B;&#x304B;&#12369;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">baden</trd>
  <trd lng="pt">banhar</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_k;&c_u;&c_n;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">k&uacute;pa&tcaron;</trd>
  <trd lng="sw">kuoga</trd>
  <trd lng="uk">&c_k;&c_u;&c_p;&c_a;&c_t;&c_i;</trd>
  <trd lng="vo">ban&ouml;n</trd>
  <trd lng="zh">&#x6D74; <pr>y&#xF9;</pr></trd>
</drv>

<drv mrk="ban.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Sin <tld/>i, fari&gcirc;i <tld/>ata:
      <ekz>
        mi <tld/>i&gcirc;as en vasta maro
        <fnt>
          <bib>SkandalJozef</bib>
          <lok>Plendoj de Jozefo arestita</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi trovi&gcirc;is <tld/>i&gcirc;anta en akvo &gcirc;is la zono
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        li iris <klr>[&#8230;]</klr> bapti tiujn, kiuj
        volis <tld/>i&gcirc;i en la akvo de l&#8217; vivo
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LXX</lok>
        </fnt>;
      </ekz>
      <ekz>
        vasta lando <tld/>i&gcirc;anta en la briloj de la suno
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap.3</lok></fnt>;
      </ekz>
      <ekz>
        kun palega viza&gcirc;o, <tld/>i&gcirc;antan en agonia &scirc;vito
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        knabet&#8217;, sinforgesinte por la hor&#8217;, knabinojn gapas <tld/>i&gcirc;antajn en rivero
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470;2</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni havis multe da malfacileco por supreniri en la akvoplenan boaton
        <klr>[&#8230;]</klr>, kvaza&ubreve; ni <tld/>i&gcirc;us en <tld/>ujo
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekdua</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dron.0i">droni</ref>,
      <ref cel="nagx.0i">na&gcirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_p;&c_a;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="br">
    <trd>kibella&ntilde;</trd>,
    <trd>kouronka&ntilde;</trd>
  </trdgrp>
  <trd lng="ca">banyar-se</trd>
  <trd lng="de">baden <klr>(intr.)</klr></trd>
  <trd lng="en">take a <ind>bath</ind></trd>
  <trd lng="es">ba&ntilde;arse</trd>
  <trd lng="fr">se <ind>baigner</ind></trd>
  <trd lng="he">&lamed;&he;&tav;&resh;&het;&ftsadi;</trd>
  <trdgrp lng="hu">
    <trd>f&uuml;rdik</trd>,
    <trd>&aacute;zik<klr tip="ind"> (folyad&eacute;kban)</klr></trd>
  </trdgrp>
  <trd lng="id">mandi</trd>
  <trdgrp lng="ja">
    <trd>&#x6C34;&#x6D74;&#12377;&#x308B;  <pr>&#12377;&#12356;&#12424;&#x304F;&#12377;&#x308B;</pr></trd>,
    <trd>&#20837;&#x6D74;&#12377;&#x308B;  <pr>&#x306B;&#12421;&#12358;&#12424;&#x304F;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>k&aogonek;pa&cacute; si&eogonek;</trd>,
    <trd>p&lstroke;awi&cacute; si&eogonek;</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_u;&c_p;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="sk">k&uacute;pa&tcaron; sa</trd>
  <trd lng="kek">achink</trd>
  <trd lng="uk">&c_k;&c_u;&c_p;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
</drv>

<drv mrk="ban.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago sin <tld/>i:
      <ekz>
        matene, post la <tld/>o, mi faras oferojn al la dio <nom>Amut</nom>
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap.23</lok></fnt>;
      </ekz>
      <ekz>
        rejunigita de la fre&scirc;a <tld/>o li sidis tie en la varma lumo de la suno
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de la jaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        forfluu infano kune kun la <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
        <trd lng="de">das Kind mit dem <ind>Bad</ind>e aussch&uuml;tten</trd>
      </ekz>
    </dif>
    <trdgrp lng="br">
      <trd>kibelladenn</trd>,
      <trd>kouronkadenn</trd>
    </trdgrp>
    <trd lng="hu">f&uuml;rd&eacute;s</trd>
    <trd lng="pl">k&aogonek;piel</trd>
  </snc>
  <snc>
    <dif>
      Flua&jcirc;o, en kiu oni <tld/>as <sncref ref="ban.0i.KOMUNE"/> a&ubreve;
      <sncref ref="ban.0i.TEK"/>:
      <ekz>
        mi preparas al vi <tld/>on kaj parfumojn
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap.1</lok></fnt>;
      </ekz>
      <ekz>
        per la ver&scirc;ilo mi &ccirc;iuvespere ricevas pluvan <tld/>on
        <fnt>
          <bib>Fab3</bib>
          <lok>Lino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la korpon <klr>[&#8230;]</klr> oni elprenis el la soda <tld/>o kaj <klr>[&#8230;]</klr>
        enbalzamigistoj kaj pastroj jam komencis la ceremoniojn
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="br">kibellad</trd>
    <trd lng="hu">f&uuml;rd&odblac;</trd>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_p;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="bo">&t_ga;&t_za;&t_shabchu;&t_ga;&t_sa;&t_tsheg;&t_pa;&t_naro;&t_tsheg;&t_kha;&t_rata;&t_shabchu;&t_tsheg;</trd>
  <trd lng="ca">bany</trd>
  <trdgrp lng="cs">
    <trd>koupel</trd>,
    <trd>l&aacute;ze&ncaron;</trd>
  </trdgrp>
  <trd lng="de">Bad</trd>
  <trd lng="en">bath</trd>
  <trd lng="es">ba&ntilde;o</trd>
  <trd lng="fr">bain</trd>
  <trd lng="he">&resh;&het;&tsadi;&he;</trd>
  <trd lng="id">mandi<klr> (n.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x6C34;&#x6D74;  <pr>&#12377;&#12356;&#12424;&#x304F;</pr></trd>,
    <trd>&#20837;&#x6D74;  <pr>&#x306B;&#12421;&#12358;&#12424;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">bad</trd>
  <trd lng="oc">banh</trd>
  <trd lng="pl">k&aogonek;piel</trd>
  <trd lng="ro">baie</trd>
  <trd lng="ru">&c_k;&c_u;&c_p;&c_a;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">k&uacute;pe&lcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_u;&c_p;&c_ib;&c_l;&c_mol;</trd>,
    <trd>&c_v;&c_a;&c_n;&c_n;&c_a;&nbsp;(&c_p;&c_r;&c_o;&c_c;&c_je;&c_d;&c_u;&c_r;&c_a;)</trd>
  </trdgrp>
  <trd lng="vo">ban</trd>
</drv>

<drv mrk="ban.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Domo, &ccirc;ambro, en kiu oni sin <tld/>as:
      <ekz>
        iri al la publikaj <tld/>ejoj
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 5</lok>
        </fnt>;
      </ekz>
      <ekz>
        li pagis por bileto kaj eniris la <tld/>ejon, elektis lokon por senvestigi sin,
        kaj &jcirc;us deprenis &ccirc;iujn vestojn kiam li <klr>[&#8230;]</klr> vidis virinon
        promenantan la&ubreve;longe la centran koridoron
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 5</lok>
        </fnt>;
      </ekz>
      <ekz>
        li mem kondukis la princon en la <tld/>ejon, asistis &ccirc;e la <tld/>o kaj el sia
        propra kesto prenis parfumojn por olei <nom>Ramzeson</nom>
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        li aran&gcirc;is grandan, kovritan per tolo, <tld/>ejon en tiu <klr>[&#8230;]</klr> lageto
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la marbordo de <nom>Aude</nom> &gcirc;uas 50 km da fajnsablaj pla&gcirc;oj kun familiaj kaj tradiciaj
        <tld/>ejoj
        <fnt>
          <aut>Marc Gassion</aut>
          <vrk><url ref="&Monato;/2003/007934.html">Aude</url></vrk>
           <bib>Monato</bib>
          <lok>2003/12, p. 18</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxambr.ban0o">ban&ccirc;ambro</ref>,
      <ref cel="nagx.0ejo">na&gcirc;ejo</ref>,
      <ref cel="plagx.0o">pla&gcirc;o</ref>,
      <ref cel="term1.0o">termo</ref>
    </refgrp>
  </snc>
  <adm>
    Indus aldoni "~&ccirc;ambro" (sub "&ccirc;ambro")
    kaj referencon al &gcirc;i &ccirc;i tie. (MB)
  </adm>
  <trdgrp lng="be">
    <trd>&c_k;&c_u;&c_p;&c_a;&c_l;&c_mol;&c_n;&c_ja;</trd>,
    <trd>&c_b;&c_a;&c_n;&c_ja;</trd>
  </trdgrp>
  <trd lng="bg">&c_b;&c_a;&c_n;&c_ja;</trd>
  <trd lng="bo">&t_ga;&t_sa;&t_nga;&t_tsheg;&t_sa;&t_pata;&t_yata;&t_naro;&t_da;&t_tsheg;</trd>
  <trdgrp lng="br">
    <trd>sal-gibella&ntilde;</trd>,
    <trd>kibelldi</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>piscina</trd>,
    <trd>balneari</trd>,
    <trd>lavabo</trd>
  </trdgrp>
  <trd lng="de">Bad</trd>
  <trd lng="en">bathroom</trd>
  <trdgrp lng="es">
    <trd>ba&ntilde;o<klr tip="ind"> (lugar)</klr></trd>,
    <trd>cuarto de ba&ntilde;o</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bain<klr tip="ind"> (lieu)</klr></trd>,
    <trd>salle de bain</trd>
  </trdgrp>
  <trd lng="he">&het;&dalet;&resh; &alef;&mem;&bet;&tet;&yod;&he;</trd>
  <trdgrp lng="hu">
    <trd>f&uuml;rd&odblac;szoba</trd>,
    <trd>f&uuml;rd&odblac;h&aacute;z</trd>
  </trdgrp>
  <trd lng="ja">&#x6D74;&#22580;  <pr>&#12424;&#x304F;&#12376;&#12423;&#12358;</pr></trd>
  <trdgrp lng="nl">
    <trd>zwembad</trd>, <trd>bad</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&lstroke;azienka</trd>,
    <trd>salon k&aogonek;pielowy</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_p;&c_a;&c_l;&c_mol;&c_n;&c_ja;</trd>,
