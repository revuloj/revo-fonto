<vortaro>
<art mrk="$Id: dokumentari.xml,v 1.10 2022/09/16 08:39:39 revo Exp $">
<kap>
  <rad>dokumentari</rad>/o
</kap>

<drv mrk="dokumentari.0o">
  <kap><tld/>o</kap>
  <snc mrk="dokumentari.0o.KIN">
    <uzo tip="fak">KIN</uzo>
    <dif>
      <ref tip="dif" cel="film.dokument0o.KIN">Dokumentfilmo</ref>:
      <ekz>
        traduki vidbendojn, filmojn kaj <tld/>ojn
        <fnt>
          <aut>R. Haveman</aut>,
          <vrk><url
          ref="http://www.tekstoj.nl/esperanto/artikoloj/terminaro.htm">Terminareto
          pri tradukado</url></vrk>,
          <lok>[2006]</lok>
        </fnt>;
      </ekz>
      <ekz>
        sen via aktiva helpo, <nom>ITV</nom> apena&ubreve; povos produkti <tld/>ojn
        pri 
        eventoj, personoj kaj historiaj atingoj de Esperanto 
        <fnt>
          <aut>&Gcirc;angalo</aut>,
          <vrk><url 
          ref="http://internacia.tv/modules/news/article.php?storyid=22">Helpu
          al Internacia Televido...</url></vrk>,
          <lok>2006-09-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_k;&c_u;&c_m;&c_e;&c_n;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_y; <ind>&c_f;&c_ib;&c_l;&c_mol;&c_m;</ind></trd>
  <trdgrp lng="de">
    <trd>Dokumentation<klr> (Film)</klr></trd>,
    <trd>Dokumentarfilm</trd>
  </trdgrp>
  <trd lng="en">documentary film</trd>
  <trd lng="fr">documentaire<klr> (film)</klr></trd>
  <trd lng="hu">dokumentumfilm</trd>
  <trd lng="nl">documentaire</trd>
  <trdgrp lng="pl">
    <trd>film <ind>dokumentalny</ind></trd>,
    <trd>dokument<klr> (film, pot.)</klr></trd>
  </trdgrp>
  <trd lng="pt">document&aacute;rio</trd>
</drv>
</art>

