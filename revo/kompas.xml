<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kompas.xml,v 1.24 2023/09/01 15:46:41 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>kompas</rad>/o
</kap>
<drv mrk="kompas.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="instru2.0o">Instrumento</ref> konsistanta el magneta montrilo, libere turni&gcirc;anta
      &ccirc;irka&ubreve; pivoto kaj konstante montranta la nordon.
      <ekz>
        ili ne plu uzis karton kaj <tld/>on sed sekvis la arbarajn
        vojetojn
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>2a parto, &ccirc;apitro 2a</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi kura&gcirc;is ekbriligi mian lampeton sur mian <tld/>on
        <fnt>
          <aut>Edgar Rice Borroughs, trad. K. R. C. Sturmer</aut>
          <vrk>Princino de Marso</vrk>
          <lok>&ccirc;apitro 23a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        socio, kiu perdis sian <tld/>on por moralo
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2008/009641.php">Kapitalista moralo</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ili kompreneble &scirc;anceli&gcirc;adis en siaj agoj, &ccirc;ar ilia cerbo malhavis la naturan
        <tld/>on
        <fnt>
          <bib>VaK</bib>
          <lok>Sesa &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi prenis tre precizan kompason, sekstanton, teleskopon
        <fnt>
          <bib>VaK</bib>
          <lok>Dekna&ubreve;a &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi trovis po&scirc;an &scirc;ip<tld/>on, kiun mi tuj alproprigis
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 47</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="naviga.0ilo">navigaciilo</ref>,
      <ref cel="orient.0ilo">orientilo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_m;&c_p;&c_a;&c_s;</trd>
  <trd lng="bg">&c_k;&c_o;&c_m;&c_p;&c_a;&c_s;</trd>
  <trd lng="ca">br&uacute;ixola</trd>
  <trd lng="cs">kompas</trd>
  <trd lng="de">Kompass</trd>
  <trd lng="en">compass</trd>
  <trd lng="es">br&uacute;jula</trd>
  <trdgrp lng="fr">
    <trd>boussole<klr> (tera navigacio)</klr></trd>,
    <trd>compas<klr> (mara a&ubreve; aera navigacio)</klr><klr tip="ind"> (de
    navigation)</klr></trd>
  </trdgrp>
  <trd lng="nl">kompas</trd>
  <trd lng="pl">kompas</trd>
  <trdgrp lng="pt">
    <trd>b&uacute;ssola</trd>
  </trdgrp>
   <trd lng="ro">busol&abreve;</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_a;&c_s;</trd>
  <trd lng="sk">kompas</trd>
</drv>
<drv mrk="kompas.K0o">
  <kap><tld lit="K"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
  </kap>
  <mlg>Pyx</mlg>
  <snc mrk="kompas.K0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Sud-hemisfera <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref>
      (<trd lng="la">Pyxis</trd>):
      <ekz>
        NGC 2613 <klr>[&#x2026;]</klr> estas galaksio en konstelacio <tld lit="K"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=112518">NGC 2613</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_K;&c_o;&c_m;&c_p;&c_a;&c_s; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#x2019;&c_je;)</klr></trd>
  <trd lng="de">Schiffskompass <klr>(Sternbild)</klr></trd>
  <trd lng="en">Pyxis</trd>
  <trd lng="fr">Boussole<klr tip="ind"> (constellation)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x7F85;&#x91DD;&#x76E4;  [<ind>&#x3089;&#x3057;&#x3093;&#x3070;&#x3093;</ind>]</trd>,
    <trd>&#x30B3;&#x30F3;&#x30D1;&#x30B9;</trd>
  </trdgrp>
  <trd lng="pl">Kompas <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Busola <klr>(constela&#x21B;ie)</klr></trd>
  <trd lng="uk">&c_k;&c_o;&c_m;&c_p;&c_a;&c_s;</trd>
</drv>
</art>
