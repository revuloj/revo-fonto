  <trd lng="br">kalonek</trd>
  <trdgrp lng="en">
    <trd>audacious</trd>,
    <trd>courageous</trd>,
    <trd>bold</trd>,
    <trd>brave</trd>,
    <trd>dauntless</trd>,
    <trd>fearless</trd>,
    <trd>intrepid</trd>,
