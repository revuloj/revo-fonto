<drv mrk="kuragx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="klr">(la objekto estas infinitivo)</uzo>
    <snc mrk="kuragx.0i.KOMUNE">
    <dif>
      Havi <tld/>on:
      <ekz>
        kiu <tld/>as rajdi sur leono
        <fnt>
          <bib>F</bib>
          <lok>&FE; 7</lok>
        </fnt>?
      </ekz>
      <ekz>
        Jesuo <klr>[&#x2026;]</klr> diris: <tld/>u, filino, via fido vin savis, kaj en tiu sama
        horo la virino resani&gcirc;is
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 9:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        neniu el la dis&ccirc;iploj <tld/>is lin demandi
        <fnt>
          <bib>NT</bib>
          <lok>Johano 21:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne <tld/>is tion diri al vi
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        al mi oni ne <tld/>as tion fari
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la akvo ne estis sufi&ccirc;e nekovrita kaj glata, por ke &scirc;i povu <tld/>i travadi &gcirc;in
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de unu patrino</lok>
        </fnt>;
      </ekz>
      <ekz>
        homo klera, kiu <tld/>as havi propran ju&gcirc;on, scias ja tre bone, ke &ccirc;io tio &ccirc;i
        estas nur senenhava pse&ubreve;do-scienca babilado
        <fnt>
          <bib>EE</bib>
          <lok>III</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_v;&c_a;&c_zh;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_s;&c_mol;&c_m;&c_je;&c_c;&c_mol;</trd>
