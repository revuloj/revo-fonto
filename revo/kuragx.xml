  </trdgrp>
  <trdgrp lng="es">
    <trd>cobarde</trd>,
    <trd>miedoso<klr> (adj. sin coraje)</klr></trd>
    <trd>temeroso</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>couard</trd>,
    <trd>l&acirc;che<klr> (adj. sans courage)</klr></trd>
