    <trd>bang<klr tip="ind"> (laf)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>boja&zacute;liwy</trd>,
    <trd>tch&oacute;rzliwy</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>timid</trd>,
    <trd>tem&abreve;tor</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_l;&c_o;&c_d;&c_u;&c_sh;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_r;&c_u;&c_s;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_o;&c_ja;&c_z;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_o;&c_ja;&c_g;&c_u;&c_z;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_l;&c_ja;&c_k;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="vo">lukinik</trd>
