<drv mrk="kuragx.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="kuragx.0igi.KOMUNE">
    <dif>
      Inspiri <tld/>on al iu; fari iun <tld/>a,
      <ref tip="vid" cel="instig.0i.KOMUNE">instigi</ref>:
      <ekz>
        kelke da feli&ccirc;aj ideoj, kiuj aperis <klr>[dum]</klr> nevola meditado,
        <tld/>igis min por plua laborado
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        donu instrukciojn al Josuo kaj fortigu kaj <tld/>igu lin, &ccirc;ar li transiros anta&ubreve;
        &ccirc;i tiu popolo
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 3:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi devi&gcirc;is skribi al vi, por <tld/>igi vin batali por la kredo
        <fnt>
          <bib>NT</bib>
          <lok>Judas 1:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i tiu re&gcirc;a ago forte ribeligis kaj <tld/>igis la popola&ccirc;on
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ekparolis al &scirc;i amike kaj kore, <tld/>igis &scirc;in
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi penas <tld/>igi min kaj faras kelkajn pa&scirc;ojn
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        ilia bonkoreco <tld/>igas min sentime paroli
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bo">&t_sa;&t_kata;&t_shabchu;&t_la;&t_tsheg;&t_la;&t_cata;&t_ga;&t_tsheg;&t_ga;&t_ta;&t_naro;&t_nga;&t_tsheg;</trd>
