<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>6</ofc>
  <rad>a&ubreve;gur</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="auxgur.0o">
  <kap><tld/>o</kap>
  <snc mrk="auxgur.0o.DIV">
    <dif>
      Natura a&ubreve; ka&ubreve;zata apera&jcirc;o rigardata kiel simbola <ref tip="super"
      cel="sign.antaux0o">anta&ubreve;signo</ref> de estonta&jcirc;o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu unua sukceso estas bona <tld/>o por la fina
        venko de nia afero.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_a;&c_d;&c_v;&c_je;&c_s;&c_mol;&c_c;&c_je;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_d;&c_k;&c_a;&c_z;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_p;&c_r;&c_y;&c_k;&c_m;&c_je;&c_t;&c_a;</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_z;&c_n;&c_a;&c_m;&c_je;&c_n;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="auxgur.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      En Romio flugo a&ubreve; kanto de birdoj interpretata kiel signo pri
      estonto.
    </dif>
  </snc>
  <trd lng="en">augury</trd>
  <trd lng="es">augurio</trd>
  <trdgrp lng="fr">
    <trd>augure <klr tip="ind">(divination)</klr></trd>,
    <trd>auspice</trd>,
    <trd>pr&eacute;sage</trd>
  </trdgrp>
</drv>

<drv mrk="auxgur.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Diveni, <ref tip="super" cel="dir.antaux0i">anta&ubreve;diri</ref>
      estonta&jcirc;on helpe de <tld/>aj signoj.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_d;&c_k;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="en">predict</trd>
  <trd lng="es">augurio</trd>
  <trdgrp lng="fr">
    <trd>augurer</trd>,
    <trd>pr&eacute;voir<klr> (augurer)</klr></trd>,
    <trd>pr&eacute;dire<klr> (augurer)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_s;&c_k;&c_a;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="auxgur.0isto">
  <kap><tld/>isto<fnt>Z</fnt></kap>
  <snc mrk="auxgur.0isto.DIV">
    <dif>
      <ref tip="super" cel="dir.antaux0isto">Anta&ubreve;diristo</ref> anoncanta
      okazonta&jcirc;ojn per interpreto de <tld/>oj:
      <ekz>
        la Fili&scirc;toj vokis la pastrojn kaj la <tld/>istojn, kaj diris: Kion
        ni faru kun la kesto de la Eternulo?
        <fnt><bib>MT</bib>, <lok>&Sa1; 6:2</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="auxgur.0isto.HIS">
    <dif>
      En Romio pastro interpretanta el la flugo a&ubreve; kanto de birdoj
      estonton.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_d;&c_k;&c_a;&c_z;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trd lng="en">soothsayer</trd>
  <trdgrp lng="fr">
    <trd>augure <klr tip="ind">(devin)</klr></trd>,
    <trd>devin</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_d;&c_s;&c_k;&c_a;&c_z;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_a;&c_v;&c_g;&c_u;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="auxgur.bon0a">
  <kap>bon<tld/>a</kap>
  <snc>
    <dif>
      Anonca, ta&ubreve;ga por feli&ccirc;iga okaza&jcirc;o, por fortuno:
      <ekz>
        oni ne elektis ankora&ubreve; bon<tld/>an daton
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 66a, volumo 2a, p.
        424a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_o;&c_b;&c_r;&c_y;</trd>,
    <trd>&c_s;&c_p;&c_r;&c_y;&c_ja;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>auspicieux</trd>,
    <trd>faste<klr> (signe)</klr></trd>,
    <trd>favorable<klr> (signe)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="auxgur.malbon0a">
  <kap>malbon<tld/>a</kap>
  <snc>
    <dif>
      Anonca, ka&ubreve;za de malfeli&ccirc;iga okaza&jcirc;o, de misfortuno:
      <ekz>
        la korvo estas malbon<tld/>a.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_d;&c_o;&c_b;&c_r;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_p;&c_r;&c_y;&c_ja;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>de mauvais <ind>augure</ind></trd>,
    <trd>d&eacute;favorable<klr> (signe)</klr></trd>,
    <trd>n&eacute;faste<klr> (signe)</klr></trd>
  </trdgrp>
</drv>
<adm>
  Kontroli la oficialecajn indikojn. La&ubreve; PIV "~i" enestas en la
  6a oficiala aldono kaj "~o" mem ne estas Z-a. (MB)
</adm>
</art>
