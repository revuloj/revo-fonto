<drv mrk="auxgur.0i">
  <kap><ofc>6</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="diven.0i">Diveni</ref>, 
      <ref tip="super" cel="dir.antaux0i">anta&ubreve;diri</ref>
      estonta&jcirc;on helpe de <tld/>aj signoj:
      <ekz>
        &gcirc;i estas ja tiu pokalo, el kiu trinkas mia sinjoro, kaj li anka&ubreve; <tld/>as sur &gcirc;i
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 44:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        li diris: <tld/>u al mi, mi petas, per via <tld/>a povoscio, kaj elvoku al mi
        tiun, pri kiu mi diros al vi
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 28:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        egipta <tld/>isto komencis <tld/>i al la gastoj ilian estontecon la&ubreve; movi&gcirc;ado de
        &ccirc;ielarke koloraj doradoj, fermitaj en kristala vazo
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni iam <tld/>is al mi, ke Romo &ccirc;esos ekzisti, kaj mi regos super la tuta Oriento
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; duonjaron mi ne <tld/>as al lia regado
        <fnt>
          <bib>SatirRak</bib>
        </fnt>;
      </ekz>
      <ekz>
        la horoskopistoj <klr>[&#x2026;]</klr> <tld/>as la estonton la&ubreve;, ekzemple, koloroj de
        fumo eliranta el uzintuboj
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk><url ref="&Steloj;monato2/006895.html">En spegulo de l' sarkasmo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        doktori&gcirc;anto ellaboris teston por <tld/>i <klr>(<ref cel="progno.0i">prognozi</ref>)</klr>, 
        &ccirc;u abiturientoj kapablos sukcese eniri universitatan studfakon
        <fnt>
          <bib>Monato</bib>
          <aut>Guido Van Damme</aut>
          <vrk><url ref="&Steloj;monato2/007287.html">Studentoj universitaton soifantaj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="profet.0i.KOMUNE">profeti<sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
      <trd><klr>(die Zukunft)</klr> vorhersagen</trd>,
      <trd><klr>(die Zukunft)</klr> deuten</trd>,
      <trd>weissagen</trd>,
      <trd>wahrsagen</trd>,
      <trd>hellsehen</trd>
    </trdgrp>
  </snc>
  <snc mrk="auxgur.0i.anonci">
    <dif>
      Kiel anta&ubreve;signo <ref tip="super" cel="anonc.0i.antausciigi">anonci</ref>:
      <ekz>
        la suno, rigardante el purblua &ccirc;ielo, <tld/>is belan tagon
        <fnt>
          <bib>KrM</bib>
          <lok>28. Tute Libera Kiel Birdo Aera</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tempo komencis boligi sian lafon &ccirc;ie, kaj <klr>[&#x2026;]</klr> en la duonpreta
        anta&ubreve;urbo oni devis scii, ke la momenta silento <tld/>as uraganon
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="promes.0i.esperigi">promesi<sncref/></ref>
    </refgrp>
    <trd lng="de">ank&uuml;ndigen</trd>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_d;&c_k;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">verhei&szlig;en</trd>
  
  <trdgrp lng="en">
    <trd>augur</trd>,
    <trd>predict</trd>,
    <trd>foretell</trd>,
    <trd>foreshadow</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>augurar</trd>,
    <trd>presagiar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>augurer</trd>,
    <trd>pr&eacute;voir<klr> (augurer)</klr></trd>,
    <trd>pr&eacute;dire<klr> (augurer)</klr></trd>
  </trdgrp>
  <trd lng="he">&lamed;&nun;&bet;&alef;</trd>
  <trdgrp lng="hu">
    <trd>j&ouml;vend&ouml;l</trd>,
    <trd>(meg)<ind>j&oacute;sol</ind></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>predire</trd>,
    <trd>presagire</trd>,
    <trd>divinare</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wr&oacute;&zdot;y&cacute;</trd>,
    <trd>wieszczy&cacute;</trd>,
    <trd>przepowiada&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_d;&c_s;&c_k;&c_a;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_v;&c_je;&c_shch;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x456;&#x449;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x440;&#x43E;&#x440;&#x43E;&#x43A;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x435;&#x440;&#x435;&#x434;&#x431;&#x430;&#x447;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x43E;&#x440;&#x43E;&#x436;&#x438;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="auxgur.0o">
  <kap><tld/>o</kap>
  <snc mrk="auxgur.0o.DIV">
    <dif>
      Ago <tld/>i; <ref tip="super" cel="sign.antaux0o">anta&ubreve;signo</ref> de estonta&jcirc;o:
      <ekz>
        la amon de la popolo oni povis pli &gcirc;uste konsideri malbona <tld/>o, kaj la
        skeptika <nom>Petronius</nom> estis samtempe supersti&ccirc;a
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj kaj son&gcirc;oj &ndash; tio ligas sin kune
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ajnas al mi, ke estis en tio ia minaco, kvaza&ubreve; <tld/>o de malfeli&ccirc;o
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        falsa <tld/>o
        <fnt>
          <bib>SatirRak</bib>
          <lok>La falsa a&ubreve;guro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> zorgumis, &ccirc;u la <tld/>oj eble ne pravi&gcirc;os estontatempe
        <fnt>
          <bib>SkandalJozef</bib>
          <lok>Plendoj de Jozefo arestita</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj de &jcirc;urnalistoj pri politiko ne tre fidindas
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="&Monato;/2012/010584.php">Kelkaj kontentas, kelkaj kritikas</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="omen.0o">omeno</ref>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_a;&c_d;&c_v;&c_je;&c_s;&c_mol;&c_c;&c_je;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_d;&c_k;&c_a;&c_z;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_p;&c_r;&c_y;&c_k;&c_m;&c_je;&c_t;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Weissagung</trd>,
      <trd>Vorzeichen</trd>
    </trdgrp><trdgrp lng="en">
      <trd>augury</trd>,
      <trd>omen</trd>,
      <trd>portent</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>augurio</trd>,
      <trd>presagio</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>augure <klr tip="ind">(divination)</klr></trd>,
      <trd>auspice</trd>,
      <trd>pr&eacute;sage</trd>
    </trdgrp>
    <trd lng="he">&alef;&vav;&gimel;&vav;&resh;&yod;</trd>
    <trd lng="hu"><klr>(j&oacute; vagy rossz)</klr> el&odblac;jel</trd>       
    <trdgrp lng="it">
      <trd>augurio</trd>,
      <trd>auspicio</trd>
      <trd>presagio</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>przepowiednia</trd>,
      <trd>omen</trd>,
      <trd>wr&oacute;&zdot;ba</trd>,
      <trd>znak</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_je;&c_d;&c_z;&c_n;&c_a;&c_m;&c_je;&c_n;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_m;&c_je;&c_t;&c_a;</trd>,
      <trd>&c_p;&c_r;&c_je;&c_d;&c_v;&c_je;&c_s;&c_t;&c_i;&c_je;</trd>
    </trdgrp>
    <trd lng="th">&#xE25;&#xE32;&#xE07;&#xE1A;&#xE2D;&#xE01;&#xE40;&#xE2B;&#xE15;&#xE38;</trd>
  </snc>
  <snc mrk="auxgur.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      En Romio, anta&ubreve;dirado per flugo a&ubreve; kanto de birdoj 
      interpretata kiel signo pri estonto.
    </dif>
    <trd lng="de">Auguration</trd>
    <trd lng="hu">j&oacute;sjel</trd>    
    <trdgrp lng="it">
      <trd>augurio</trd>,
      <trd>presagio</trd>
    </trdgrp>
    <trd lng="pl">fatum</trd>
    <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_s;&c_k;&c_a;&c_z;&c_a;&c_n;&c_i;&c_je;
    &c_p;&c_o; &c_p;&c_o;&c_l;&c_jo;&c_t;&c_u;
    &c_p;&c_t;&c_i;&c_c;</trd>
  </snc>
  <snc mrk="auxgur.0o.EVI">
    <dif>
      <ref tip="dif" cel="auxgur.0isto"><tld lit="A"/>isto</ref>:
      <ekz>
        en la Samosa Hera-templo la <tld/>oj divenas el la orbitoj de la steloj
        <fnt>
          <bib>SatirRak</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x437;&#x43D;&#x430;&#x43A;&#x430;</trd>,
    <trd>&#x43F;&#x440;&#x438;&#x43A;&#x43C;&#x435;&#x442;&#x430;</trd>
  </trdgrp>
</drv>


<drv mrk="auxgur.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="auxgur.0isto.DIV">
    <dif>
      <ref tip="super" cel="dir.antaux0isto">Anta&ubreve;diristo</ref>
      anoncanta okazonta&jcirc;ojn per interpreto de anta&ubreve;signoj:
      <ekz>
        <klr>[&ccirc;u]</klr> trovi&gcirc;i inter vi iu <klr>[&#x2026;]</klr> <tld/>isto, anta&ubreve;diristo,
        magiisto, sor&ccirc;isto
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 18:10</lok>
        </fnt>?
      </ekz>
      <ekz>
        la Fili&scirc;toj vokis la pastrojn kaj la <tld/>istojn, kaj
        diris: kion ni faru kun la kesto de la Eternulo
        <fnt><bib>MT</bib>, <lok>&Sa1; 6:2</lok></fnt>?
      </ekz>
      <ekz>
        sa&gcirc;ulo kaj <tld/>isto, kiu scias anta&ubreve;legi homajn sortojn kaj anta&ubreve;diri
        estontecan
        <fnt>
          <bib>QuV</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="profet.0o">profeto</ref>
    <trdgrp lng="de">
      <trd>Wahrsager</trd>,
      <trd>Weissager</trd>,
      <trd>Hellseher</trd>
    </trdgrp><trdgrp lng="it">
      <trd>indovino</trd>, <trd>profeta</trd>,
      <trd>astrologo<klr> (indovino)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>wr&oacute;&zdot;bita</trd>,
      <trd>wieszcz</trd>,
      <trd>prorok</trd>
    </trdgrp>
  </snc>
  <snc mrk="auxgur.0isto.HIS">
    <dif>
      En Romio <ref tip="super" cel="pastr.0o.gxenerale">pastro</ref> 
      interpretanta el la flugo a&ubreve; kanto de birdoj estonton.
    </dif>
    <trd lng="de">Augur</trd>
    <trdgrp lng="it">
      <trd>augure</trd>
    </trdgrp>
    <trd lng="pl">wyrocznia</trd>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_d;&c_k;&c_a;&c_z;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="en">
    <trd>diviner</trd>,
    <trd>soothsayer</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>augure <klr tip="ind">(devin)</klr></trd>,
    <trd>devin</trd>
  </trdgrp>
    <trd lng="he">&lamed;&#x5B0;&bet;&#x5B7;&shin;&#x5C2;&#x5B5;&resh;</trd>
  <trdgrp lng="hu">
      <trd>(mad&aacute;r)<ind>j&oacute;s</ind></trd>,
      <trd>j&ouml;vend&odblac;mond&oacute;</trd>
    </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_d;&c_s;&c_k;&c_a;&c_z;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_a;&c_v;&c_g;&c_u;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x430;&#x432;&#x491;&#x443;&#x440;</trd>,
    <trd>&#x43F;&#x440;&#x43E;&#x432;&#x456;&#x441;&#x43D;&#x438;&#x43A;</trd>,
    <trd>&#x432;&#x43E;&#x440;&#x43E;&#x436;&#x431;&#x438;&#x442;</trd>
  </trdgrp>
</drv>

<drv mrk="auxgur.bon0a">
  <kap>bon<tld/>a</kap>
  <snc>
    <dif>
      Anoncanta feli&ccirc;igan estonta&jcirc;on:
      <ekz>
        oni ne elektis ankora&ubreve; bon<tld/>an daton
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 66a, volumo 2a, p.
        424a</lok></fnt>;
      </ekz>
      <ekz>
        la decido estas bon<tld/>a
        <fnt>
          <bib>Monato</bib>
          <aut>Alberto Garc&iacute;a Fumero</aut>
          <vrk><url ref="&Monato;/2009/009707.php">Servo alternative fiksita</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="esper.0iga">esperiga</ref>,
      <ref cel="favor.0a.prosperiga">favora<sncref/></ref>,
      <ref cel="promes.0o">promesoplena</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_o;&c_b;&c_r;&c_y;</trd>,
    <trd>&c_s;&c_p;&c_r;&c_y;&c_ja;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">verhei&szlig;ungsvoll</trd>
  <trd lng="en">of good omen</trd>
  <trdgrp lng="fr">
    <trd>auspicieux</trd>,
    <trd>faste<klr> (signe)</klr></trd>,
    <trd>favorable<klr> (signe)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>b&iacute;ztat&oacute;</trd>,
    <trd>&iacute;g&eacute;retes</trd>,
    <trd>kedvez&odblac;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dobrze <ind>rokuj&aogonek;cy</ind></trd>,
    <trd>dobrze si&eogonek; <ind>zapowiadaj&aogonek;cy</ind></trd>,
    <trd>pod dobr&aogonek; <ind>gwiazd&aogonek;</ind></trd>,
    <trd>pod szcz&eogonek;&sacute;liw&aogonek;
    <ind>gwiazd&aogonek;</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_b;&c_r;&c_y;&c_j;<klr tip="amb">
    (&c_z;&c_n;&c_a;&c_k;,
    &c_p;&c_r;&c_je;&c_d;&c_z;&c_n;&c_a;&c_m;&c_je;&c_n;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;,
    &c_p;&c_r;&c_i;&c_m;&c_je;&c_t;&c_a;)</klr></trd>
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="auxgur.malbon0a">
  <kap>malbon<tld/>a</kap>
  <snc>
    <dif>
      Anoncanta malbonan estonta&jcirc;on:
      <ekz>
        ho, &scirc;i diris, kia malbon<tld/>a birdo vi estas!
        <fnt>
          <bib>Fab4</bib>
          <lok>Anneto</lok>
        </fnt>;
      </ekz>
      <ekz>
        sub tiu frunto de duondio trovi&gcirc;is viza&gcirc;o de simio <klr>[&#x2026;]</klr>, al Ligia li
        ek&scirc;ajnis malbon<tld/>a, sed anta&ubreve; &ccirc;io abomena
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        el la kunpremitaj makzeloj eli&gcirc;adis malbon<tld/>a grinco
        <fnt>
          <bib>QuV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fatal.0a">fatala</ref>,
      <ref cel="sinistr.0a">sinistra</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_d;&c_o;&c_b;&c_r;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_p;&c_r;&c_y;&c_ja;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>unheilvoll</trd>,
    <trd>omin&ouml;s</trd>,
    <trd>unheilschwanger</trd>,
    <trd>verh&auml;ngnisvoll</trd>
  </trdgrp><trd lng="en">of ill omen</trd>
  <trdgrp lng="fr">
    <trd>de mauvais <ind>augure</ind></trd>,
    <trd>d&eacute;favorable<klr> (signe)</klr></trd>,
    <trd>n&eacute;faste<klr> (signe)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>balj&oacute;s</trd>,
    <trd>omin&oacute;zus</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>z&lstroke;owr&oacute;&zdot;bny</trd>,
    <trd>&zacute;le <ind>rokuj&aogonek;cy</ind></trd>,
    <trd>&zacute;le si&eogonek; <ind>zapowiadaj&aogonek;cy</ind></trd>,
    <trd>pod z&lstroke;&aogonek; <ind>gwiazd&aogonek;</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_o;&c_h;&c_o;&c_j;<klr tip="amb">
    (&c_z;&c_n;&c_a;&c_k;,
    &c_p;&c_r;&c_je;&c_d;&c_z;&c_n;&c_a;&c_m;&c_je;&c_n;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;,
    &c_p;&c_r;&c_i;&c_m;&c_je;&c_t;&c_a;)</klr></trd>,
