<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: asklepios.xml,v 1.1 2026/01/25 18:45:17 afido Exp $">
    <kap>
      <rad>Asklepios</rad>/o,
      <var><kap><rad var="i">Asklepi</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>,
      <var><kap><rad var="E">Eskulap</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
    </kap>

<drv mrk="asklepios.0o">
  <kap><tld/>o, <var><kap><tld var="i"/>o</kap></var>, <var><kap><tld var="E"/>o</kap></var></kap>
  <uzo tip="fak">MIT</uzo>
  <snc>
    <dif>
      <klr>(la&ubreve; la greka kaj roma mitologio)</klr> Dio de kuracarto:
      <ekz>
        estu al vi favoraj &ccirc;iuj dioj, precipe <tld/>o
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld var="i"/>o ne instruis &ccirc;i tiun specon de medicino al siaj idoj
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Tri</lok>
        </fnt>;
      </ekz>
      <ekz>
        je <tld var="E"/>o, mi malkovros al la tuta mondo vian fripona&jcirc;on
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Asklepios</trd>
    <trd lng="en">Asclepius</trd>
    <trd lng="fr">Ascl&eacute;pios</trd>
    <trd lng="ru">&c_A;&c_s;&c_k;&c_l;&c_je;&c_p;&c_i;&c_j;</trd>
  </snc>
</drv>

  </art>

