<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vasal.xml,v 1.26 2024/04/13 22:21:57 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vasal</rad>/o <fnt><bib>Far1</bib></fnt>
</kap>
<drv mrk="vasal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vasal.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      <ref tip="dif" cel="feuxd.0ulo.HIS">Fe&ubreve;dulo</ref>:
      <ekz>
        la &leftquot;imperiestro&rightquot; disdonis fe&ubreve;dojn al siaj lin obeantaj <tld/>oj
        <fnt><aut>Zlatko Ti&scaron;ljar</aut>,
        <vrk><url ref="&Monato;/2000/006910.php">Post morto -- nova sorto</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/04, p. 10</lok>
        </fnt>;
      </ekz>
      <ekz>
        nun mi lo&gcirc;as en arbaro, / &scirc;telas, rabas pro neceso, /
        kaj dividas &ccirc;ion inter / mi kaj miaj dek <tld/>oj
        <fnt>
          <aut>trad. P.-J. Doumen</aut>,
          <vrk>Karlo kaj Elegasto</vrk>,
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne ekzistis tie unu sola Etiopio, sed granda amaso de neordigitaj homoj, kaj
        tial hodia&ubreve; ili estas niaj <tld/>oj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        en 1615 <nom>Hasekura Cunenaga</nom>, <tld/>o de <nom>Date
        Masamune</nom>, potenca daimio en la nord-orienta Japanio, marveturis tra Meksiko al
        Hispanio kaj Vatikano, kie lin a&ubreve;dience ricevis la papo Pa&ubreve;lo la 5a
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2015/011398.php">Sekretaj kristanoj, &ccirc;u veraj
          kristanoj?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_a;&c_s;&c_a;&c_l;</trd>
    <trd lng="br">gwaz<klr tip="amb"> (dindan un aotrou)</klr></trd>
    <trd lng="ca">vassall</trd>
    <trdgrp lng="cs">
      <trd>len&iacute;k</trd>,
      <trd>man</trd>,
      <trd>vazal</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Vasall</trd>,
      <trd>Lehnsmann</trd>
    </trdgrp>
    <trd lng="el">&ypsilon;&pi;&omikron;&tau;&epsilon;&lambda;&eta_ton;&sigma_fina;</trd>
    <trd lng="en">vassal</trd>
    <trdgrp lng="fa">
      <trd>&a_w;&a_A;&a_s;&a_A;&a_l;</trd>,
      <trd>&a_kh;&a_r;&a_A;&a_j;&f_g;&a_z;&a_A;&a_r;</trd>
    </trdgrp>
    <trd lng="fr">vassal</trd>
    <trdgrp lng="hu">
      <trd>h&udblac;b&eacute;res</trd>,
      <trd>vazallus</trd>
    </trdgrp>
    <trd lng="nl">vazal</trd>
    <trd lng="pt">vassalo</trd>
    <trd lng="ru">&c_v;&c_a;&c_s;&c_s;&c_a;&c_l;</trd>
    <trdgrp lng="sk">
      <trd>vazal</trd>,
      <trd>z&aacute;visl&aacute; osoba</trd>
    </trdgrp>
  </snc>
  <snc mrk="vasal.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Regno, &scirc;tato plene dependanta de alia;
      <tld/>&scirc;tato:
      <ekz>
        Israelo restas <tld/>o de Va&scirc;ingtono, kiu detruas Irakon kaj nun preparas
        Israelon por bategi Iranon
        <fnt>
          <bib>Monato</bib>
          <aut>Husejn Al-Amily</aut>
          <vrk><url ref="&Monato;/2008/009594.php">Israelo 60-jara (4)</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_a;&c_s;&c_a;&c_l;</trd>,
      <trd>&c_h;&c_a;&c_l;&c_u;&c_j;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Vasallenstaat</trd>,
      <trd>Satellitenstaat</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x5BB6;&#x81E3;  <pr>&#x304B;&#12375;&#12435;</pr></trd>,
    <trd>&#x81E3;&#x4E0B;  <pr>&#12375;&#12435;&#x304B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
     <trd>wasal</trd>,
     <trd>podw&lstroke;adny</trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_a;&c_s;&c_a;&#x43B;</trd>
</drv>
</art>
