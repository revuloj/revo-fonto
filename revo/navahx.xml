<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>nava&hcirc;</rad>/o <fnt><bib>GDFE</bib></fnt>
</kap>
<drv mrk="navahx.0o">
  <kap><tld/>o</kap>
  <snc mrk="navahx.0o.ETN">
    <dif>
      <ref tip="super" cel="indian.0o.ETN">Indiano</ref> el apa&ccirc;-lingva
      popolo de nordo migrinta Novmeksikon, Arizonon kaj Utahon, tie
      vivinta per kultivo kaj bredo, nun dise lo&gcirc;anta en usonaj urboj:
      <ekz>
        <tld lit="N"/>oj kaj Apa&ccirc;oj transmigris suden
        <fnt><bib>Viki</bib>, <lok>&leftquot;Na-Denea Familio&rightquot;,
        2005-06-16</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>Navaho<klr> (subst.)</klr></trd>,
    <trd>Navajo<klr> (subst.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="navahx.0a">
  <kap><tld/>a</kap>
  <snc mrk="navahx.0a.ETN">
    <dif>
      Rilata al la <tld/>oj.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>navaho<klr> (adj.)</klr></trd>,
    <trd>navajo<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>
</art>

