<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>peset</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="peset.0o">
  <kap><tld/>o</kap>
  <snc mrk="peset.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="unu.mon0o.EKON">Monunuo</ref>
      de Hispanio de 1869 &gcirc;is la alveno de la e&ubreve;ro:
      <ekz>
        en butiko vi pagus pli ol 30 <tld/>ojn por tiu robo
        <fnt>
          <aut>trad. J. Sarasua</aut>,
          <vrk><url ref="http://filmoj.net/la-pedalauxto">
          La Pedala&ubreve;to</url></vrk>,
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="peset.0o.monero">
    <dif>
      Iu el diversaj hispanaj moneroj.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_s;&c_e;&c_t;&c_a;</trd>
  <trd lng="bg">&c_p;&c_je;&c_s;&c_je;&c_t;&c_a;</trd>
  <trd lng="de">Pesete</trd>
  <trd lng="en">peseta</trd>
  <trd lng="es">peseta</trd>
  <trd lng="fr">peseta</trd>
  <trd lng="hu">pezeta</trd>
  <trd lng="nl">peseta</trd>
  <trd lng="pl">peseta</trd>
  <trd lng="pt">peseta</trd>
  <trd lng="ru">&c_p;&c_je;&c_s;&c_je;&c_t;&c_a;</trd>
</drv>
</art>
