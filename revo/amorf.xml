<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>amorf</rad>/a
</kap>
<drv mrk="amorf.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Sen regula kaj difinita <ref tip="ant" cel="strukt.0o">strukturo</ref>:
      <ekz>
        <nom>Zoe</nom> nomas sin artisto, sed &ccirc;u &scirc;i iam sukcesas vendi <klr>[&#8230;]</klr> siajn
        arta&jcirc;ojn, ne estas klare <klr>[ili estas]</klr> abstraktaj pentra&jcirc;oj, sed
        precipe <klr>[&#8230;]</klr> glubildoj, faritaj el <tld/>aj kunmeta&jcirc;oj de ruba&jcirc;o
        <klr>[&#8230;]</klr> nek puraj nek allogaj
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="https://www.monato.net/2009/009680.php"
              >Proksimaj kaj malproksimaj najbaroj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_m;&c_o;&c_r;&c_f;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_ja;&c_s;&c_f;&c_o;&c_r;&c_m;&c_je;&c_n;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">amorf</trd>
  <trdgrp lng="cs">
    <trd>amorfn&iacute;</trd>,
    <trd>beztvar&yacute;</trd>,
    <trd>nekrystalick&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>amorph</trd>,
    <trd>formlos</trd>
  </trdgrp>
  <trd lng="en">amorphous</trd>
  <trd lng="es">amorfo</trd>
  <trd lng="fr">amorphe</trd>
  <trd lng="nl">amorf</trd>
  <trd lng="pl">amorficzny</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_m;&c_o;&c_r;&c_f;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_f;&c_o;&c_r;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>beztvar&yacute;</trd>,
    <trd>amorfn&yacute;</trd>
  </trdgrp>
</drv>
</art>
