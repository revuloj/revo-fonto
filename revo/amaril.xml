<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>amarilid</rad>/o
</kap>
<drv mrk="amaril.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="amaril.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Amaryllis</trd>), ornama bulba planto, kun grandaj,
      bonodoraj floroj.  
    </dif>
  </snc>
  <trd lng="de">Amaryllis</trd>
</drv>
<drv mrk="amaril.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de unukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Amaryllidaceae</trd>).
    </dif>
    <refgrp tip="sub">
      <ref cel="amaril.0o"><tld/>o</ref>,
      <ref cel="galant.0o">galanto</ref>,
      <ref cel="narcis.0o">narciso</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Amaryllisgew&auml;chse</trd>
</drv>
</art>
