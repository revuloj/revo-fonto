<vortaro>
<art mrk="$Id$">
<kap>
  <rad>antracit</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="antrac.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Plej bonkvalita speco de <ref tip="super"
      cel="karb.ter0o">terkarbo</ref>,
      konsistanta el preska&ubreve; pura karbono kaj liveranta grandan varmon.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_t;&c_r;&c_a;&c_c;&c_y;&c_t;</trd>
  <trd lng="ca">antracita</trd>
  <trd lng="de">Anthrazit<ind>kohle</ind></trd>
  <trd lng="en">anthracite</trd>
  <trd lng="es">antracita</trd>
  <trd lng="fr">anthracite</trd>
  <trd lng="hu">antracit</trd>
  <trd lng="ru">&c_a;&c_n;&c_t;&c_r;&c_a;&c_c;&c_i;&c_t;</trd>
</drv>
</art>
