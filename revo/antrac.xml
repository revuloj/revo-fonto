<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>antracit</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="antrac.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Plej bonkvalita speco de <ref tip="super" cel="karb.ter0o">terkarbo</ref>,
      konsistanta el preska&ubreve; pura <ref tip="prt" cel="karbon.0o">karbono</ref>
      kaj liveranta grandan varmon:
      <ekz>
        pro efiko de premo kaj temperaturo <klr>[&#8230;]</klr> torfo (&ccirc;. 50&#8239;% el C) i&gcirc;as
        sinsekve brunkarbo (&ccirc;. 65 &gcirc;is 78&#8239;% el C) kiel lignito kun 65&#8239;% da C,
        &scirc;tonkarbo (&ccirc;. 80 &gcirc;is 92&#8239;% el C), <tld/>o (&ccirc;. 96&#8239;% el C) kaj finfine pura
        karbona mineralo, nome grafito
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=158033"
              >Sedimenta roka&jcirc;o</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="koaks.0o">koakso</ref>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_t;&c_r;&c_a;&c_c;&c_y;&c_t;</trd>
  <trd lng="ca">antracita</trd>
  <trd lng="cs">antracit</trd>
  <trd lng="de">Anthrazit<ind>kohle</ind></trd>
  <trd lng="en">anthracite</trd>
  <trd lng="es">antracita</trd>
  <trd lng="fa">&a_A_madda;&a_n;&a_t;&a_r;&a_A;&a_s;&f_y;&a_t;</trd>
  <trd lng="fr">anthracite</trd>
  <trd lng="hu">antracit</trd>
  <trd lng="ru">&c_a;&c_n;&c_t;&c_r;&c_a;&c_c;&c_i;&c_t;</trd>
  <trd lng="sk">antracit</trd>
</drv>
</art>
