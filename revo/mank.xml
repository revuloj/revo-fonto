<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mank.xml,v 1.35 2023/09/02 06:04:09 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>mank</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="mank.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="mank.0i.KOMUNE">
    <dif>
      Tute <ref tip="sin" cel="est.mal0i">malesti</ref>, 
      <ref tip="sin" cel="est.for0i">foresti</ref> a&ubreve; ne esti en sufi&ccirc;a kvanto
      a&ubreve; grado la&ubreve; la bezono:
      <ekz>
        &ccirc;io plenumi&gcirc;is por vi, ne <tld/>is el &gcirc;i e&ccirc; unu vorto
        <fnt>
          <bib>MT</bib>
          <lok>Josuo 23:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>is el la servantoj de <nom>David</nom> dek na&ubreve; viroj
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 2:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>as al li ankora&ubreve; nur la re&gcirc;eco
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 18:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u <tld/>as al mi frenezuloj, ke vi alkondukis &ccirc;i tiun
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 21:15</lok>
        </fnt>?
      </ekz>
      <ekz>
        al lia ma&scirc;kiraso <tld/>is unu maniko
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne multe <tld/>is, ke ili elparolu <klr>[tion kvankam]</klr> ili ne estas
        permesite protesti
        <fnt>
          <bib>Fab3</bib>
          <lok>Du pavimbatiloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        al vi <tld/>as ne spirito, sed pripensemeco
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        al junuloj povas manki takto
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>as al ili la &gcirc;usta komprenado
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        beleco al &scirc;i ne <tld/>as, kontra&ubreve;e, neniu povas esti tiel bela kaj alloga
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili ne glacii&gcirc;as, sed ne multe <tld/>as &gcirc;is tio
        <fnt>
          <bib>Fab1</bib>
          <lok>Cikonioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        al posedanto de metio <tld/>as nenio
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        al malri&ccirc;ulo infanoj ne <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>se la sorto vin batas, mokantoj ne <tld/>as</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
        <trd lng="de">wer den Schaden hat braucht f&uuml;r den <ind>Spott</ind> nicht sorgen</trd>
      </ekz>
      <ekz>
        <ind>al la papero ne <tld/>as tolero</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>.
        <trd lng="de">Papier ist <ind>geduldig</ind></trd>
        <trd lng="lat">epistula non erubescit</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_r;&c_a;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_je; &c_h;&c_a;&c_p;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_s;&c_u;&c_t;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_l;&c_i;&c_p;&c_s;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_o;&c_t;&c_s;&c_malmol;&c_s;&c_t;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_n;&c_je; &c_d;&c_o;&c_s;&c_t;&c_i;&c_g;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>mankout</trd>,
    <trd>ober <ind>diouer</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>chyb&ecaron;t</trd>,
    <trd>chyb&ecaron;t</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>fehlen</trd>,
    <trd>mangeln</trd>
  </trdgrp>
  <trd lng="en">be <ind>lacking</ind></trd>
  <trdgrp lng="es">
     <trd>faltar</trd>,
     <trd>carecer</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire d&eacute;faut</trd>,
    <trd>manquer<klr> (faire d&eacute;faut)</klr></trd>
  </trdgrp>
  <trd lng="hu">hi&aacute;nyzik</trd>
  <trdgrp lng="ja">
    <trd>&#x6B20;&#x3051;&#x3066;&#x3044;&#x308B;  [<ind>&#x304B;&#x3051;&#x3066;&#x3044;&#x308B;</ind>]</trd>,
    <trd>&#x4E0D;&#x8DB3;&#x3057;&#x3066;&#x3044;&#x308B;  [<ind>&#x3075;&#x305D;&#x304F;&#x3057;&#x3066;&#x3044;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">ontbreken</trd>
  <trdgrp lng="pl">
    <trd>brakowa&cacute;</trd>,
    <trd>by&cacute; <ind>nieobecnym</ind></trd>,
    <trd>niedostawa&cacute;</trd>
  </trdgrp>  
  <trd lng="pt">faltar</trd>
  <trd lng="ro">lipsi</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_d;&c_o;&c_s;&c_t;&c_a;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_je; &c_h;&c_v;&c_a;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_s;&c_u;&c_t;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ch&yacute;ba&tcaron;</trd>,
    <trd>postr&aacute;da&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_r;&c_a;&#x43A;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43D;&c_je; &c_v;&c_i;&c_s;&c_t;&c_a;&c_ch;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_u;&c_t;&c_i; &c_v;&c_ib;&c_d;&c_s;&c_u;&c_t;&#x43D;&c_ib;&#x43C;</trd>
  </trdgrp>
</drv>

<drv mrk="mank.0ede">
  <kap><tld/>e de</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="mank.0ede.KOMUNE">
    <dif>
      <ref tip="super" cel="sen.0">Sen</ref> sufi&ccirc;a dispono de:
      <ekz>
        &scirc;i apena&ubreve; povis paroli, <tld/>e de spiro
        <fnt>
          <bib>ChB</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e de pruvo, <klr>[&scirc;i]</klr> ne povis tion publikigi
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi nenion komprenis, <tld/>e de la bazaj scioj
        <fnt>
          <bib>CKv</bib>
        </fnt>;
      </ekz>
      <ekz>
        la tradukistoj &gcirc;in imagas la&ubreve; la
        publikiga&jcirc;oj, <tld/>e de
        kontaktoj kun la vivanta <klr>[&#x2026;]</klr> lingvo
        <fnt><bib>BonaLingvo</bib>, <lok>p. 88a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">dre ziouer a</trd>
  <trdgrp lng="es">
    <trd>en <ind>ausencia de</ind></trd>,
    <trd>a <ind>falta de</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>en l'<ind>absence de</ind></trd>,
    <trd>&agrave; <ind>d&eacute;faut de</ind></trd>
  </trdgrp>
  <trd lng="hu">hi&aacute;ny&aacute;ban</trd>
  <trdgrp lng="pl">
    <trd>przy <ind>braku</ind></trd>,
    <trd>w zwi&aogonek;zku z <ind>brakiem</ind></trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>&icirc;n absen&#x21B;a</trd>,
    <trd>datorit&abreve; lipsei</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;
    &c_n;&c_je;&c_i;&c_m;&c_je;&c_n;&c_i;&c_je;&c_m;</trd>,
    <trd>&c_n;&c_je; &c_i;&c_m;&c_je;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="mank.0o">
  <kap><tld/>o</kap>
  <snc mrk="mank.0o.malsuficxo">
    <dif>
      Malesto a&ubreve; nesufi&ccirc;o de tio, kio estas bezonata:
      <ekz>
        <tld/>o de pano
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 3:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        la popolo tie suferis pro <tld/>o de akvo
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 17:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        malsato kaj soifo kaj <klr>[&#x2026;]</klr> <tld/>o de &ccirc;io
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 28:48</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de eduko
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 5:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        multaj pereas pro <tld/>o de justeco
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 13:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        absoluta <tld/>o de singardo
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de respekto
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        viajn krimojn ili klarigas al si kiel erarojn de juneco, kiel <tld/>on de
        pripenso
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        infano havas limigitajn bezonojn <klr>[kaj tial &gcirc;in]</klr> indiferentigas multaj
        aferoj, kies posedo a&ubreve; <tld/>o estas gravega por matura homo
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        plej dan&gcirc;era malsano estas <tld/>o de sa&gcirc;o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bezon.0o">bezono</ref>,
      <ref cel="brecx.0o">bre&ccirc;o</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_n;&c_je;&c_d;&c_a;&c_h;&c_o;&c_p;</trd>,
      <trd>&c_n;&c_ja;&c_s;&c_t;&c_a;&c_ch;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="bg">
      <trd>&c_l;&c_i;&c_p;&c_s;&c_a;</trd>,
      <trd>&c_o;&c_t;&c_s;&c_malmol;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>,
      <trd>&c_n;&c_je;&c_d;&c_o;&c_s;&c_t;&c_i;&c_g;</trd>
    </trdgrp>
    <trd lng="de">Fehlen</trd>
    <trdgrp lng="es">
      <trd>escasez</trd>,
      <trd>ausencia</trd>
    </trdgrp>
    <trd lng="hu">hi&aacute;ny</trd>    
    <trdgrp lng="ru">
      <trd>&c_n;&c_je;&c_d;&c_o;&c_s;&c_t;&c_a;&c_ch;&c_a;</trd>,
      <trd>&c_n;&c_je;&c_h;&c_v;&c_a;&c_t;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="mank.0o.difekto">
    <uzo tip="klr">(abs)</uzo>
    <dif>
      Malesto de necesa <ref tip="ant" cel="kvalit.0o">kvalito</ref>, malperfekta&jcirc;o, malhava&jcirc;o,
      <ref cel="difekt.0o">difekto</ref>:
      <ekz>
        <klr>[ili]</klr> malgra&ubreve; siaj <tld/>oj (kiu ne havas ilin!) faras al la lando
        grandegajn servojn
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi esperas, ke nia trupo estas libera de tiaj
        <tld/>oj
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        liberigita de militservo, li volas almena&ubreve; iom ripari sian <tld/>on tiurilate
        <fnt>
          <bib>ViV</bib>
        </fnt>;
      </ekz>
      <ekz>
        prezentu liajn <tld/>ojn en tia lumo, ke ili aperu kiel makuloj
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        iliaj <tld/>oj preska&ubreve; estis meritoj
        <fnt>
          <bib>HsT</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tiajn]</klr> <tld/>ojn en komparo kun lingvo natura lingvo arta posedas
        neniajn
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="makul.0o.FIG">makulo<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_n;&c_je;&c_d;&c_a;&c_h;&c_o;&c_p;</trd>,
      <trd>&c_z;&c_a;&c_g;&c_a;&c_n;&c_a;</trd>
    </trdgrp>
    <trd lng="bg">&c_n;&c_je;&c_d;&c_o;&c_s;&c_t;&c_a;&c_t;&c_malmol;&c_k;</trd>
    <trdgrp lng="de">
      <trd>Makel</trd>,
      <trd>Manko</trd>,
      <trd>Fehler <klr>(Mangel)</klr></trd>
    </trdgrp>
    <trd lng="es">deficiencia</trd>
    <trd lng="hu">hi&aacute;nyoss&aacute;g</trd>    
    <trd lng="ru">&c_n;&c_je;&c_d;&c_o;&c_s;&c_t;&c_a;&c_t;&c_o;&c_k;</trd>
  </snc>
  <trd lng="bo">&t_ma;&t_tsheg;&t_-a;&t_da;&t_nga;&t_sa;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trdgrp lng="br">
    <trd>diouer</trd>,
    <trd>mank</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>deficit</trd>,
    <trd>nedostatek</trd>
  </trdgrp>
  <trd lng="de">Mangel</trd>
  <trdgrp lng="es">
     <trd>falta</trd>,
     <trd>carencia</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>carence</trd>,
    <trd>manque</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4E0D;&#x8DB3;  [<ind>&#x3075;&#x305D;&#x304F;</ind>]</trd>,
    <trd>&#x6B20;&#x4E4F;  [<ind>&#x3051;&#x3064;&#x307C;&#x3046;</ind>]</trd>,
    <trd>&#x6B20;&#x843D;  [<ind>&#x3051;&#x3064;&#x3089;&#x304F;</ind>]</trd>,
    <trd>&#x6B20;&#x70B9;  [<ind>&#x3051;&#x3063;&#x3066;&#x3093;</ind>]</trd>,
    <trd>&#x6B20;&#x9665;  [<ind>&#x3051;&#x3063;&#x304B;&#x3093;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">gebrek</trd>
  <trdgrp lng="pl">
    <trd>brak</trd>,
    <trd>niedob&oacute;r</trd>,
    <trd>defekt</trd>
  </trdgrp> 
  <trd lng="ro">lips&abreve;</trd>
  <trdgrp lng="sk">
    <trd>deficit</trd>,
    <trd>nedostatok</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_s;&c_u;&c_t;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&#x43D;&c_je;&c_s;&c_t;&c_a;&c_ch;&c_a;</trd>,
    <trd>&#x43D;&c_je;&c_d;&#x43E;&c_s;&c_t;&c_a;&c_ch;&c_a;</trd>,
    <trd>&c_b;&c_r;&c_a;&#x43A;</trd>,
    <trd>&#x43D;&c_je;&c_d;&#x43E;&#x43B;&c_ib;&#x43A;</trd>
  </trdgrp>
</drv>

<drv mrk="mank.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="mank.0igi.KOMUNE">
    <dif>
      Fari ke io <tld/>as, preterlasi:
      <ekz>
        li <tld/>igis nenion el &ccirc;io, kion la Eternulo
        ordonis
        <fnt>
          <bib>MT</bib>
          <lok>Josuo 11:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne <tld/>igu <klr>(<ref cel="las.preter0i">preterlasu</ref>)</klr> 
        e&ccirc; unu vorton
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 26:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        la embargo pri armiloj <tld/>igas &ccirc;iajn modernajn armilojn
        <fnt>
          <bib>MD17</bib>
        </fnt>.
      </ekz>
    </dif>
  <trdgrp lng="de">
    <trd><klr>(an etwas) </klr><ind>fehlen</ind> lassen</trd>,
    <trd>auslassen</trd>,
    <trd>weglassen</trd>
  </trdgrp></snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_p;&c_u;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_p;&c_u;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_m;&c_ib;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>opomenout</trd>,
    <trd>vynechat</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>omitir</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>n&eacute;gliger</trd>,
    <trd>oublier<klr> (n&eacute;gliger)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kihagy</trd>,
    <trd>n&eacute;lk&uuml;l&ouml;z <klr tip="ind">(nem vesz
    figyelembe)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x304A;&#x308D;&#x305D;&#x304B;&#x306B;&#x3059;&#x308B;</trd>,
    <trd>&#x6B20;&#x3051;&#x3055;&#x305B;&#x308B;  [<ind>&#x304B;&#x3051;&#x3055;&#x305B;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">achterwege laten</trd>
  <trdgrp lng="pl">
    <trd>opu&sacute;ci&cacute;</trd>,
    <trd>pomin&aogonek;&cacute;</trd>,
    <trd>powodowa&cacute; <ind>brak</ind></trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>abandona</trd>,
    <trd>omite</trd>,
