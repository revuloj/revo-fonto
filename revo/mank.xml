      <trd>komplett</trd>
    </trdgrp><trdgrp lng="es">
      <trd>integral</trd>,
      <trd>completo</trd>
    </trdgrp>
    <trd lng="fr">int&eacute;gral</trd>
    <trd lng="hu">hi&aacute;nytalan</trd>
    <trdgrp lng="pl">
      <trd>ca&lstroke;kowity</trd>,
      <trd>kompletny</trd>,
      <trd>pe&lstroke;ny</trd>
    </trdgrp>  
