      <trd>tot</trd>,
      <trd>complet</trd>
    </trdgrp> 
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_l;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_k;&c_o;&c_m;&c_p;&c_l;&c_je;&c_k;&c_t;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="mank.sen0a.sendifekta">
    <dif>
      <ref tip="dif" cel="difekt.sen0a">Sendifekta</ref>, 
      tute <ref tip="super" cel="bon.0a.tauga">bona</ref>, <ref tip="sin" cel="perfek.0a">perfekta</ref>:
      <ekz>
        filologie sen<tld/>a redakto
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        impreso pri sen<tld/>a solidareco de la albanaj laboristoj
        <fnt>
          <bib>ChB</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi ne trovas mian rezonon sen<tld/>a
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni havas bonan, perfekte kompetentan kunlaboranton, homon kun <klr>[&#x2026;]</klr>
        sen<tld/>a kono de &ccirc;iuj dosieroj, kiujn oni traktas
        <fnt>
          <bib>CKv</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_b;&c_je;&c_z;&c_z;&c_a;&c_g;&c_a;&c_n;&c_n;&c_y;</trd>,
      <trd>&c_b;&c_je;&c_z;&c_d;&c_a;&c_k;&c_o;&c_r;&c_n;&c_y;</trd>
    </trdgrp>      
    <trd lng="bg">&c_b;&c_je;&c_z;&c_u;&c_p;&c_r;&c_je;&c_ch;&c_je;&c_n;</trd>
