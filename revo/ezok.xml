<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ezok</rad>/o
</kap>
<drv mrk="ezok.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="ezoked.0oj"><tld/>edoj</ref>
      (<trd lng="la">Esox</trd>), man&gcirc;ebla riverfi&scirc;o:
      <ekz>
        la plej grandaj <tld/>oj, kapteblaj per forksimilaj iloj, pezas
        dekon da kilogramoj, sed pli ofte eblas kapti
        2-4-kilogramajn<fnt><aut>Saliko</aut>: <vrk>Fi&scirc;kapti
        ekscite</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 10a, p.
        21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_ch;&c_u;&c_p;&c_a;&c_k;</trd>
  <trd lng="bg">&c_shch;&c_u;&c_k;&c_a;</trd>
  <trd lng="ca">llu&ccedil; de riu</trd>
  <trd lng="de">Hecht</trd>
  <trdgrp lng="en">
    <trd>jack</trd>,
    <trd>pike</trd>
  </trdgrp>
  <trd lng="fr">brochet</trd>
  <trd lng="hu">csuka</trd>
  <trd lng="pl">szczupak</trd>
  <trd lng="pt">l&uacute;cio</trd>
  <trd lng="ru">&c_shch;&c_u;&c_k;&c_a;</trd>
  <trd lng="sv">g&auml;dda</trd>
</drv>

<drv mrk="ezok.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO"
      lst="voko:zoologiaj_ordoj">Ordo</ref> de <ref tip="malprt"
      cel="fisx.osto0oj">ostofi&scirc;oj</ref>
      (<trd lng="la">Esociformes</trd>).
    </dif>
  </snc>
  <trd lng="ca">es&ograve;cids</trd>
  <trd lng="de">Hechtartige Fische <klr tip="ind">(Ordnung)</klr></trd>
  <trd lng="hu">csukaalak&uacute;ak</trd>
  <trd lng="pl">szczupakokszta&lstroke;tne</trd>
  <trd lng="ru">&c_shch;&c_u;&c_k;&c_o;&c_v;&c_i;&c_d;&c_n;&c_y;&c_je;
  <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
</drv>
</art>
