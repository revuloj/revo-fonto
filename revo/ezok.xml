<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>ezok</rad>/o
</kap>
<drv mrk="ezok.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="ezok.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Esox</trd>), man&gcirc;ebla riverfi&scirc;o.
    </dif>
  </snc>
  <trd lng="de">Hecht</trd>
  <trd lng="fr">brochet</trd>
</drv>
<drv mrk="ezok.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="ezok.0oformaj"><tld/>oformaj
      fi&scirc;oj</ref> (<trd lng="la">Esocidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Hechte <klr tip="ind">(Familie)</klr></trd>
</drv>
<drv mrk="ezok.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="fisx.0oj">fi&scirc;oj</ref>
      (<trd lng="la">Esociformes</trd>).
    </dif>
  </snc>
  <trd lng="de">Hechtartige Fische <klr tip="ind">(Ordnung)</klr></trd>
</drv>
</art>
