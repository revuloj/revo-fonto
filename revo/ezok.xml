<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ezok.xml,v 1.33 2023/11/07 20:34:12 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ezok</rad>/o
</kap>
<drv mrk="ezok.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
       <ref tip="lst" cel="fisx.rabo0o" lst="voko:fi&scirc;oj">Rabofi&scirc;o</ref>,
      vivanta en nesala akvo, kun longa cilindroforma korpo,
      anasbeksimila kapo kaj dors- kaj anusna&gcirc;iloj tre
      malanta&ubreve;aj 
      permesantaj rapidajn ekmovojn kaj direkto&scirc;an&gcirc;ojn;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio <ref tip="malprt" cel="ezoked.0oj"><tld/>edoj</ref>
      (<trd lng="la">Esox</trd>):
      <ekz>
        la plej grandaj <tld/>oj, kapteblaj per forksimilaj iloj, pezas
        dekon da kilogramoj, sed pli ofte eblas kapti
        2-4-kilogramajn<fnt><aut>Saliko</aut>: <vrk>Fi&scirc;kapti
        ekscite</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 10a, p.
        21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_ch;&c_u;&c_p;&c_a;&c_k;</trd>
  <trd lng="bg">&c_shch;&c_u;&c_k;&c_a;</trd>
  <trd lng="ca">llu&ccedil; de riu</trd>
  <trd lng="cs">&scaron;tika</trd>
  <trd lng="de">Hecht</trd>
  <trdgrp lng="en">
    <trd>jack</trd>,
    <trd>pike</trd>
  </trdgrp>
  <trd lng="fr">brochet</trd>
  <trd lng="hu">csuka</trd>
  <trd lng="ja">&#x30AB;&#x30EF;&#x30AB;&#x30DE;&#x30B9;</trd>
  <trd lng="pl">szczupak</trd>
  <trd lng="pt">l&uacute;cio</trd>
  <trd lng="ru">&c_shch;&c_u;&c_k;&c_a;</trd>
  <trd lng="sk">&scaron;&tcaron;uka</trd>
  <trd lng="sv">g&auml;dda</trd>
  <trd lng="uk">&c_shch;&c_u;&#x43A;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x9577;&#x77DB; <pr>ch&#xE1;ngm&#xE1;o</pr></trd>,
    <trd>&#x72D7;&#x9B5A; <pr>g&#x1D2;uy&#xFA;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ezok.0oformaj">
  <kap><tld/>oformaj</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref> de <ref tip="malprt" cel="fisx.osto0oj">ostofi&scirc;oj</ref>
      (<trd lng="la">Esociformes</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_ch;&c_u;&c_p;&c_a;&c_k;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja; <klr>(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="ca">es&ograve;cids</trd>
  <trd lng="cs">&scaron;tiky <klr>(rod)</klr></trd>
  <trd lng="de">Hechtartige Fische <klr tip="ind">(Ordnung)</klr></trd>
  <trd lng="fr">Esociformes</trd>
  <trd lng="hu">csukaalak&uacute;ak</trd>
  <trd lng="pl">szczupakokszta&lstroke;tne</trd>
  <trd lng="ru">&c_shch;&c_u;&c_k;&c_o;&c_v;&c_i;&c_d;&c_n;&c_y;&c_je;
  <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trd lng="sk">&scaron;&tcaron;uky</trd>
</drv>
</art>
