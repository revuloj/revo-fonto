<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>9</ofc>
  <rad>anus</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="anus.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">ANA</uzo>
    <dif>
      Malferma&jcirc;o, kiu trovi&gcirc;as &ccirc;e la fino de la
      <ref cel="kanal.nutro0o">nutrokanalo</ref>, kaj tra kiu eliras
      la <ref cel="fek.0ajxo">feka&jcirc;oj</ref>.
      <!--
      Ekstremo de la intesto el kiu eli&gcirc;as la feka&jcirc;oj.
      -->
    </dif>
    <refgrp tip="sin">
      <ref cel="tru.postajxo0o">posta&jcirc;otruo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_n;&c_u;&c_s;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_n;&c_ib; &c_p;&c_r;&c_a;&c_h;&c_o;&c_d;</trd>
  </trdgrp>
  <trd lng="ca">anus</trd>
  <trdgrp lng="de">
    <trd>Anus</trd>,
    <trd>Arschloch<klr> [vulg.]</klr></trd>
  </trdgrp>
  <trd lng="en">anus</trd>
  <trd lng="es">ano</trd>
  <trd lng="fr">anus</trd>
  <trd lng="he">&pe;&yod; &he;&tet;&bet;&ayin;&tav;</trd>
  <trdgrp lng="hu">
    <trd>v&eacute;gb&eacute;lny&iacute;l&aacute;s</trd>,
    <trd>anus</trd>
  </trdgrp>
  <trd lng="it">ano</trd>
  <trd lng="nl">anus</trd>
  <trd lng="pl">odbyt</trd>
  <trd lng="pt">&acirc;nus</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_d;&c_n;&c_i;&c_j; &c_p;&c_r;&c_o;&c_h;&c_o;&c_d;</trd>,
    <trd>&c_a;&c_n;&c_u;&c_s;</trd>
  </trdgrp>
  <trd lng="tr">an&uuml;s</trd>
</drv>

<drv mrk="anus.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al la <tld/>o.
    </dif>
  </snc>
  <trd lng="en">anal</trd>
  <trd lng="fr">anal</trd>
  <trd lng="it">anale</trd>
</drv>
</art>
