<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vindas.xml,v 1.20 2024/06/26 22:30:20 revo Exp $">
<kap>
  <rad>vindas</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="vindas.0o">
  <kap><tld/>o</kap>
  <snc mrk="vindas.0o.TEK">
  <uzo tip="fak">TEK</uzo>
  <uzo tip="fak">MAR</uzo>
    <dif>
      Horizontala <ref tip="super" cel="vincx.0o">vin&ccirc;o</ref>,
      kiu ne envolvas la &scirc;nuregon a&ubreve; &ccirc;enon
      <klr>(vd <ref cel="kapsta.0o">kapstano</ref>)</klr>, uzata
      precipe por la ankra &ccirc;eno:
      <ekz>
        meze de la <tld/>oj kaj ma&scirc;inoj
        <fnt>
          <aut>Albert Camus, trad. Michel Duc Goninaz</aut>,
          <vrk>La Fremdulo</vrk>,
          <lok>S.A.T., 1993, p. 36</lok>
        </fnt>;
      </ekz>
      <ekz>
        en landoj kie ne estas montaroj, la glispara&scirc;utistoj povas uzi <tld/>on
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Glispara%C5%9Dutado">Glispara&scirc;utado</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kapsta.0o">kapstano</ref>,
      <ref cel="krik.0o">kriko</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_a;&c_sh;&c_p;&c_ib;&c_l;&c_mol;</trd>
  <trd lng="ca">molinet<klr> (n&agrave;ut.)</klr></trd>
  <trd lng="cs">lodn&iacute; rump&aacute;l</trd>
  <trd lng="de">Bratspill</trd>
  <trd lng="en">windlass</trd>
  <trd lng="fr">treuil</trd>
  <trdgrp lng="hu">
    <trd>gugora <klr tip="ind">(horgonycs&ouml;rl&odblac;)</klr></trd>,
    <trd>j&aacute;rg&aacute;ny <klr tip="ind">(horgonycs&ouml;rl&odblac;)</klr></trd>
  </trdgrp>
  <trd lng="nl">windas</trd>
  <trd lng="pl">winda kotwiczna</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_a;&c_sh;&c_p;&c_i;&c_l;&c_mol;</trd>,
    <trd>&c_k;&c_a;&c_b;&c_je;&c_s;&c_t;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="sk">navijak horizont&aacute;lny <klr>(pre kotvy)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x44F;&#x43A;&c_ib;&c_r;&#x43D;&c_a; &#x43B;&c_je;&c_b;&c_ib;&c_d;&#x43A;&c_a;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_sh;&#x43F;&c_i;&#x43B;&#x44C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8F46; <pr>l&#xF9;</pr></trd>,
    <trd>&#x8F98; <pr>l&#xF9;</pr></trd>,
    <trd>&#x8F64; <pr>l&#xFA;</pr></trd>,
    <trd>&#x8F73; <pr>l&#xFA;</pr></trd>,
    <trd>&#x7D5E;&#x8ECA; <pr>ji&#x1CE;och&emacron;</pr></trd>,
    <trd>&#x7EDE;&#x8F66; <pr>ji&#x1CE;och&emacron;</pr></trd>
  </trdgrp>
</drv>
</art>
