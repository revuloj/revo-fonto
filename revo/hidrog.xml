<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hidrog.xml,v 1.34 2023/11/15 17:08:00 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>hidrogen</rad>/o
</kap>
<drv mrk="hidrog.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <mlg kod="IUPAC">H</mlg>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="1">elemento</ref> kun
      atomnumero 1, atompezo 1,00797; 
      <ref tip="super" cel="gas.0o">gaso</ref> plej malpeza kaj
      senkolora:
      <ekz>
        la elektrolizo malkomponas akvon je <tld/>o kaj 
        <ref tip="vid" cel="oksige.0o">oksigeno</ref>
        <fnt>
        <aut>Stanis&lstroke;aw Lem, trd. Valentin Melnikov</aut>:
        <vrk>Luna nokto (radia a&ubreve;da&jcirc;o)</vrk>,
        <lok>
        <url ref="http://www.esperanto.mv.ru/Arkivoj/UTF8/Fantastiko/Luna_nokto.html">
        Arkivo de "Survoje", en la lasta alineo
        </url>
        </lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_d;&c_a;&c_r;&c_o;&c_d;</trd>
  <trd lng="bg">&c_v;&c_o;&c_d;&c_o;&c_r;&c_o;&c_d;</trd>
  <trd lng="br">hidrogen</trd>
  <trd lng="ca">hidrogen</trd>
  <trdgrp lng="cs">
    <trd>hydrogenium</trd>,
    <trd>vod&#xED;k</trd>
  </trdgrp>
  <trd lng="de">Wasserstoff</trd>
  <trd lng="el">&ypsilon;&delta;&rho;&omikron;&gamma;&omikron_ton;&ny;&omikron;</trd>
  <trd lng="en">hydrogen</trd>
  <trd lng="es">hidr&oacute;geno</trd>
  <trdgrp lng="fa">
    <trd>&a_h;&f_y;&a_d;&a_r;&a_w;&f_zh;&a_n;</trd>,
    <trd>&a_y_hamza;&f_y;&a_d;&a_r;&a_w;&f_zh;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">hydrog&egrave;ne</trd>
  <trd lng="hu">hidrog&eacute;n</trd>
  <trd lng="id">hidrogen</trd>
  <trd lng="it">idrogeno</trd>
  <trd lng="ja">&#x6C34;&#x7D20;  <pr>&#x3059;&#x3044;&#x305D;</pr></trd>
  <trd lng="la">Hydrogenium</trd>
  <trd lng="nl">waterstof</trd>
  <trd lng="pl">wod&oacute;r</trd>
  <trd lng="pt">hidrog&ecirc;nio</trd>
  <trd lng="ru">&c_v;&c_o;&c_d;&c_o;&c_r;&c_o;&c_d;</trd>
  <trd lng="sk">vod&#xED;k</trd>
  <trd lng="sv">v&auml;te</trd>
  <trd lng="tr">hidrojen</trd>
  <trdgrp lng="uk">
    <trd>&#x433;&#x456;&#x434;&#x440;&#x43E;&#x433;&#x435;&#x43D; (&#x445;&#x456;&#x43C;&#x456;&#x447;&#x43D;&#x438;&#x439; &#x435;&#x43B;&#x435;&#x43C;&#x435;&#x43D;&#x442;)</trd>,
    <trd>&#x432;&#x43E;&#x434;&#x435;&#x43D;&#x44C; (&#x43F;&#x440;&#x43E;&#x441;&#x442;&#x430; &#x440;&#x435;&#x447;&#x43E;&#x432;&#x438;&#x43D;&#x430;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6C22; <pr>q&#x12B;ng</pr></trd>,
    <trd>&#x6C22;&#x6C14; <pr>q&#x12B;ngq&#xEC;</pr></trd>,
    <trd>&#x6C14;&#x8106; <pr>q&#xEC;cu&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>
