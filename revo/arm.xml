<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: arm.xml,v 1.45 2024/12/12 02:03:44 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>arm</rad>/o
    </kap>

    <drv mrk="arm.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="arm.0i.MIL">
        <uzo tip="fak">MIL</uzo>
	<dif>
	  Provizi per rimedoj ta&ubreve;gaj por batali respektive protekti sin:
          <ekz>
            sur la stratoj videblis <tld/>itaj soldatoj kaj policanoj
            <fnt>

          <aut>Julian Modest</aut>
          <vrk><url ref="&Monato;/2001/007140.html">Vetiha</url></vrk>
           <bib>Monato</bib>
          <lok>2001/02, p. 26</lok>
        </fnt>;
          </ekz>
	  <ekz>
	    <tld/>i soldatojn per pafiloj;
	  </ekz><ekz>
	    per fal&ccirc;iloj <tld/>itaj kamparanoj;
	  </ekz>
          <ekz>
            soldatoj <tld/>itaj &gcirc;isdente
            <fnt>
             <aut>Bardhyl Selimi</aut>
              <vrk><url ref="&Monato;/2001/007205.html">Kosovo:
              Voja&gcirc;impresoj, somero 2000</url></vrk>
              <bib>Monato</bib>
              <lok>2001/01, p. 26</lok>

            </fnt>;
          </ekz>
          <ekz>
            sin <tld/>i kontra&ubreve; la tuta maro da mizeroj
            <fnt>
              <aut>L. L. Zamenhof</aut>
              <vrk>Hamleto, el Hamleto</vrk>
            </fnt>.
          </ekz>
	</dif>
        <ref tip="vid" cel="arme.0o">armeo</ref>
        <trd lng="be">&c_u;&c_z;&c_b;&c_r;&c_o;&c_j;&c_v;&c_a;&c_c;&c_mol;</trd>
	<trdgrp lng="de">
	  <trd>bewaffnen</trd>,
          <trd>r&uuml;sten</trd>,
          <trd>ausr&uuml;sten</trd>,
          <trd>wappnen</trd>
	</trdgrp>
        <trd lng="es">armar <klr tip="amb">(proveer con
        armas)</klr></trd>
	<trd lng="hu">felfegyverez</trd>
        <trd lng="nl">bewapenen</trd>
        <trdgrp lng="pl">
          <trd>zbroi&cacute;</trd>,
          <trd>uzbroi&cacute;</trd>
        </trdgrp>
	<trd lng="sk">vyzbroji&tcaron;</trd>
	<trd lng="ru">&c_v;&c_o;&c_o;&c_r;&c_u;&c_zh;&c_a;&c_t;&c_mol;</trd>
      </snc>
      <snc mrk="arm.0i.TEK">
	<uzo tip="fak">TEK</uzo>
	<dif>
	  Plifortigi per diversaj rimedoj, subtraba&jcirc;o, la&ubreve;cela ilo, kc:
	  <ekz>
	    konstruo el <ref cel="beton.sxtal0o"><tld/>ita
	      betono</ref>;
	  </ekz><ekz>
	    per ne<tld/>ita <klr>(nuda, senlorna)</klr> okulo<fnt>B</fnt>.
	  </ekz>
	</dif>
        <trdgrp lng="be">
           <trd>&c_u;&c_z;&c_m;&c_a;&c_c;&c_n;&c_ja;&c_c;&c_mol;</trd>,
           <trd>&c_a;&c_r;&c_m;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
        </trdgrp>
	<trdgrp lng="de">
	  <trd>armieren</trd>,
	  <trd>r&uuml;sten</trd>
	</trdgrp>
        <trd lng="es">fortalecer</trd>
	<trdgrp lng="hu">
	  <trd>megvasal<klr tip="amb"> (meger&odblac;s&iacute;t)</klr></trd>,
	  <trd>meger&odblac;s&iacute;t<klr tip="amb"> (pl. vasal&aacute;ssal)</klr></trd>
	</trdgrp>
	<trd lng="ru">&c_a;&c_r;&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
          </snc>
      <trdgrp lng="cs">
        <trd>ozbrojit</trd>,
        <trd>vyzbrojit</trd>,
        <trd>zbrojit</trd>
      </trdgrp>
      <trd lng="fr">armer</trd>
      <trd lng="he">&#x5DB;&#x5DC;&#x5D9; &#x5E0;&#x5E9;&#x5E7;</trd>
  <trd lng="it">armare</trd>
      <trdgrp lng="ja">
    <trd>&#x6B66;&#x88C5;&#12373;&#x305B;&#x308B;  <pr>&#12406;&#x305D;&#12358;&#12373;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x88C5;&#30002;&#12377;&#x308B;  <pr>&#x305D;&#12358;&#12371;&#12358;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
        <trd>ozbroji&tcaron;</trd>,
        <trd>ozbroji&tcaron;, vyzbroji&tcaron;</trd>
      </trdgrp>
      <trdgrp lng="uk">
    <trd>&#x43E;&c_z;&c_b;&c_r;&#x43E;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_a;&c_r;&#x43C;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

    <drv mrk="arm.0o">
      <kap><tld/>o<fnt>Z</fnt></kap>
      <uzo tip="stl">ARK</uzo>
      <snc mrk="arm.0o.ARM">
        <uzo tip="fak">MIL</uzo>
        <dif>
          <ref tip="dif" cel="arm.0ilo"><tld lit="A"/>ilo</ref>:
	  <ekz>
	    ne servas larmo anstata&ubreve; <tld/>o<fnt><bib>PrV</bib></fnt>.
	  </ekz>
        </dif>
	<rim>
	  Tia uzo de la vorto <tld/>o estas tre malofta, kvankam Zamenhofa.
	</rim>
        <trd lng="zh">&#x5FC5;&#35201;&#x624B;&#x6BB5; <pr>b&#xEC;y&#xE0;osh&#x1D2;udu&#xE0;n</pr></trd>
</snc>
      <trd lng="be">&c_z;&c_b;&c_r;&c_o;&c_ja;</trd>
      <trdgrp lng="cs">
        <trd>ozbrojen&iacute;</trd>,
        <trd>zbra&ncaron;</trd>
      </trdgrp>
      <trdgrp lng="de">
    	<trd>Waffe</trd>, <trd>R&uuml;stzeug</trd>
          </trdgrp>
      <trd lng="es">arma</trd>
      <trd lng="he">&#x5E0;&#x5B6;&#x5E9;&#x5C1;&#x5B6;&#x5E7;</trd>
      <trd lng="hu">fegyver</trd>
      <trd lng="it">arma</trd>
      <trd lng="ja">&#x6B66;&#22120;  <pr>&#12406;&#x304D;</pr></trd>
      <trd lng="nl">wapen</trd>
      <trdgrp lng="pl">
        <trd>bro&nacute;</trd>,
        <trd>uzbrojenie</trd>
      </trdgrp>
      <trdgrp lng="ru">
    	<trd>&c_v;&c_o;&c_o;&c_r;&c_u;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>,
    	<trd>&c_o;&c_r;&c_u;&c_zh;&c_i;&c_je;</trd>
      </trdgrp>
      <trd lng="sk">zbra&ncaron;</trd>
    </drv>

    <drv mrk="arm.0ilo">
      <kap><tld/>ilo</kap>
      <snc mrk="arm.0ilo.ARM">
	<tezrad fak="MIL"/>
        <uzo tip="fak">MIL</uzo>
	<dif>
	  &Ccirc;iuspeca ilo, uzebla por atake <ref cel="vund.0i">vundi</ref>,
	  <ref cel="mort.0igi">mortigi</ref>, detrui, <ref cel="minac.0i">minaci</ref>
	  pri tia atako, a&ubreve; <ref cel="defend.0i">defendi</ref>
	  kontra&ubreve; &gcirc;i:
	  <ekz>
	    prenu &scirc;ildon kaj <tld/>ilojn, kaj stari&gcirc;u, por helpi
	    min
	    <fnt><bib>MT</bib>, <lok>&Psa; 35:2</lok></fnt>;
	  </ekz><ekz>
	    prenu do nun viajn <tld/>ilojn, vian sagujon kaj vian pafarkon,
	    kaj iru sur la kampon kaj &ccirc;asu por mi &ccirc;asa&jcirc;on
	    <fnt><bib>MT</bib>, <lok>&Gen; 27:3</lok></fnt>;
	  </ekz><ekz>
	    doni <tld/>ilojn en la manojn de niaj malamikoj<fnt>Z</fnt>;
	  </ekz><ekz>
	    iris kun <nom>David</nom> &ccirc;irka&ubreve; kvarcent viroj, kaj ducent
	    restis &ccirc;e la <tld/>ilaro
	    <fnt><bib>MT</bib>, <lok>&Sa1; 25:13</lok></fnt>;
	  </ekz><ekz>
	    <uzo tip="stl">FIG</uzo>
	    aplombaj mensogoj estas iliaj preferataj <tld/>iloj.
	  </ekz>
	</dif>
	<refgrp tip="sub">
	  <ref cel="batal.0ilo">batalilo</ref>.
	</refgrp>
	<rim>
	  <tld lit="A"/>iloj servas precipe por batali, sed anka&ubreve;
	  por &ccirc;asi kaj fizike ekzerci&gcirc;i.
	</rim>
        <trd lng="zh">&#x6B66;&#22120; <pr>w&#x1D4;q&#xEC;</pr></trd>
</snc>
      <trd lng="be">&c_z;&c_b;&c_r;&c_o;&c_ja;</trd>
      <trd lng="cs">zbra&ncaron;</trd>
      <trd lng="de">Waffe</trd>
      <trd lng="es">arma</trd>
      <trd lng="fr">arme</trd>
      <trd lng="he">&#x5E0;&#x5B6;&#x5E9;&#x5C1;&#x5B6;&#x5E7;</trd>
      <trd lng="hu">fegyver</trd>
      <trd lng="it">arma</trd>
      <trdgrp lng="ja">
         <trd>&#x6B66;&#22120;  <pr>&#12406;&#x304D;</pr></trd>,
         <trd>&#20853;&#22120;  <pr>&#12408;&#12356;&#x304D;</pr></trd>
      </trdgrp>
      <trd lng="nl">wapen</trd>
      <trdgrp lng="pl">
         <trd>bro&nacute;</trd>,
         <trd>or&eogonek;&zdot;</trd>,
         <trd>uzbrojenie</trd>
      </trdgrp>
      <trd lng="ru">&c_o;&c_r;&c_u;&c_zh;&c_i;&c_je;</trd>
      <trd lng="sk">zbra&ncaron;</trd>
      <trd lng="uk">&c_z;&c_b;&c_r;&#x43E;&#x44F;</trd>
</drv>
    <drv mrk="arm.0ilaro">
      <kap><tld/>ilaro</kap>
      <snc mrk="arm.0ilaro.ARM">
        <uzo tip="fak">MIL</uzo>
	<dif>
	  Tuto de la <tld/>iloj:
	  <ekz>
	    iris kun <nom>David</nom> &ccirc;irka&ubreve; kvarcent viroj, kaj ducent
	    restis &ccirc;e la <tld/>ilaro
	    <fnt><bib>MT</bib>, <lok>&Sa1; 25:13</lok></fnt>;
	  </ekz><ekz>
	    <uzo tip="stl">FIG</uzo>
	    surmetu la tutan <tld/>ilaron de Dio, por ke vi povu
	    kontra&ubreve;stari al la artifikoj de la diablo
	    <fnt><bib>NT</bib>, <lok>&Efe; 6:11</lok></fnt>.
	  </ekz>
	</dif>
	<ref tip="vid" cel="arsena.0o.armilaro">arsenalo</ref>
      </snc>
      <trdgrp lng="be">
    	<trd>&c_r;&c_y;&c_sh;&c_t;&c_u;&c_n;&c_a;&c_k;</trd>,
    	<trd>&c_u;&c_z;&c_b;&c_r;&c_a;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
      </trdgrp>
      <trdgrp lng="cs">
        <trd>arzen&aacute;l</trd>,
        <trd>v&yacute;zbroj</trd>,
        <trd>zbran&ecaron;</trd>,
        <trd>zbroj</trd>
      </trdgrp>
      <trd lng="es">armamento</trd>
      <trd lng="fr">armement</trd>
      <trd lng="hu">fegyverzet</trd>
      <trdgrp lng="it">
         <trd>armamento</trd>,
         <trd>armamentario</trd>
      </trdgrp>
      <trd lng="nl">wapenuitrusting</trd>
      <trdgrp lng="pl">
         <trd>bro&nacute;</trd>,
         <trd>or&eogonek;&zdot;</trd>,
         <trd>uzbrojenie</trd>
      </trdgrp>
      <trd lng="ru">&c_v;&c_o;&c_o;&c_r;&c_u;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
      <trd lng="sk">v&yacute;zbroj</trd>
    </drv>

    <drv mrk="arm.0ilejo">
      <kap><tld/>ilejo,
      <var><kap><tld/>ejo</kap></var>,
      <var><kap><tld/>iltenejo</kap></var>
      </kap>
      <snc mrk="arm.0ilejo.MIL">
        <uzo tip="fak">MIL</uzo>
	<dif>
	  Provizejo de <tld/>iloj:
	  <ekz>
	    <nom>&Hcirc;izkija</nom> <klr>[&#8230;]</klr> montris al ili sian trezorejon
	    <klr>[&#8230;]</klr> kaj sian tutan <tld/>ilejon
	    <fnt><bib>MT</bib>, <lok>&Re2; 21:13</lok></fnt>;
	  </ekz>
          <ekz>
	    via kolo estas kiel la turo de <nom>David</nom>, kiu estas konstruita
	    kiel <tld/>iltenejo
	    <fnt><bib>MT</bib>, <lok>&Alt; 4:4</lok></fnt>.
	  </ekz>
	</dif>
	<ref tip="sin" cel="arsena.0o.armilejo">arsenalo</ref>
      </snc>
      <trdgrp lng="be">
        <trd>&c_z;&c_b;&c_r;&c_o;&c_j;&c_n;&c_ja; <klr tip="amb">(&c_s;&c_k;&c_l;&c_a;&c_d;)</klr></trd>,
        <trd>&c_a;&c_r;&c_s;&c_e;&c_n;&c_a;&c_l;</trd>
      </trdgrp>
      <trd lng="cs">zbrojnice</trd>
      <trd lng="de">Waffenkammer</trd>
      <trdgrp lng="es">
            <trd>armer&iacute;a</trd>,
            <trd>arsenal</trd>
          </trdgrp>
      <trd lng="fr">armurerie</trd>
      <trd lng="he">&#x5E0;&#x5B4;&#x5E9;&#x5C1;&#x5E7;&#x5B4;&#x5D9;&#x5B8;&#x5D4;</trd>
      <trdgrp lng="hu">
    	<trd>fegyvert&aacute;r</trd>,
    	<trd>fegyverrakt&aacute;r</trd>,
    	<trd>fegyverszoba</trd>
      </trdgrp>
      <trd lng="it">armeria</trd>
      <trdgrp lng="ja">
         <trd>&#x6B66;&#22120;&#x5EAB;  <pr>&#12406;&#x304D;&#12371;</pr></trd>,
         <trd>&#20853;&#22120;&#x5EAB;  <pr>&#12408;&#12356;&#x304D;&#12371;</pr></trd>
      </trdgrp>
      <trd lng="nl">wapenkamer</trd>
      <trdgrp lng="pl">
        <trd>arsena&lstroke;</trd>,
        <trd>zbrojownia</trd>
      </trdgrp>
      <trdgrp lng="ru">
    	<trd>&c_a;&c_r;&c_s;&c_je;&c_n;&c_a;&c_l;</trd>,
    	<trd>&c_s;&c_k;&c_l;&c_a;&c_d; &c_o;&c_r;&c_u;&c_zh;&c_i;&c_ja;</trd>
      </trdgrp>
      <trd lng="sk">zbrojnica</trd>
      <trdgrp lng="uk">
         <trd>&c_s;&#x43A;&#x43B;&c_a;&c_d; &c_z;&c_b;&c_r;&#x43E;&c_ji;</trd>,
         <trd>&c_a;&c_r;&c_s;&c_je;&#x43D;&c_a;&#x43B;</trd>
       </trdgrp>
  </drv>

    <drv mrk="arm.0ajxo">
      <kap><tld/>a&jcirc;o</kap>
      <snc mrk="arm.0ajxo.armilo">
        <uzo tip="fak">MIL</uzo>
        <dif>
	  <ref tip="super" cel="arm.0ilo"><tld lit="A"/>ilo</ref>,
	  portata por &scirc;irmi la korpon:
	  <ekz>
	    stari&gcirc;u en kaskoj, akrigu la lancojn, surmetu sur vin
	    <tld/>a&jcirc;ojn
	    <fnt><bib>MT</bib>, <lok>&Jer; 46:4</lok></fnt>.
	  </ekz>
        </dif>
	<ref tip="sin" cel="arm.sxirm0o">&scirc;irm<tld/>o</ref>
	<refgrp tip="vid">
	  <ref cel="kiras.0o">kiraso</ref>,
	  <ref cel="sxild.0o">&scirc;ildo</ref>
	</refgrp>
        <trdgrp lng="be">
           <trd>&c_b;&c_r;&c_o;&c_n;&c_mol;</trd>,
           <trd>&c_p;&c_a;&c_n;&c_c;&c_y;&c_r;</trd>
        </trdgrp>
	<trd lng="de">R&uuml;stung</trd>
	<trd lng="es">blindaje</trd>
	<trd lng="fr">armure</trd>
	<trd lng="hu">p&aacute;nc&eacute;lzat</trd>
        <trd lng="it">armatura</trd>
        <trdgrp lng="pl">
           <trd>pancerz</trd>,
           <trd>armatura</trd>,
           <trd>os&lstroke;ona</trd>,
           <trd>zbroja</trd>
        </trdgrp>
	<trdgrp lng="ru">
	  <trd>&c_b;&c_r;&c_o;&c_n;&c_ja;</trd>,
	  <trd>&c_d;&c_o;&c_s;&c_p;&c_je;&c_h;</trd>
	</trdgrp>
        <trd lng="zh">&#x76D4;&#30002; <pr>ku&#x12B;ji&#x1CE;</pr></trd>
   </snc>

      <snc mrk="arm.0ajxo.TEK">
	<uzo tip="fak">TEK</uzo>
	<dif>
	  Malmola kovra&jcirc;o, ekzemple metala, servanta por fortikigi
	  kablon kaj protekti &gcirc;in kontra&ubreve; me&hcirc;anikaj
	  dama&gcirc;oj.
	</dif>
        <trd lng="be">&c_a;&c_p;&c_l;&c_jo;&c_t;&c_k;&c_a; <klr>(&c_k;&c_a;&c_b;&c_e;&c_l;&c_ja;)</klr></trd>
	<trd lng="es">armadura<klr tip="ind"> (de un cable)</klr></trd>
	<trd lng="fr">armature<klr tip="ind"> (d'un c&acirc;ble)</klr></trd>
	<trd lng="hu">k&aacute;belp&aacute;nc&eacute;l</trd>
        <trd lng="it">armatura</trd>
        <trdgrp lng="pl">
          <trd>pancerz</trd>,
          <trd>os&lstroke;ona</trd>
        </trdgrp>
      </snc>
        <trd lng="he">&#x5E9;&#x5C1;&#x5B4;&#x5E8;&#x5D9;&#x5D5;&#x5B9;&#x5DF;</trd>
        <trdgrp lng="ja">
           <trd>&#30002;&#20881;  <pr>&#x304B;&#12387;&#12385;&#12421;&#12358;</pr></trd>,
           <trd>&#12424;&#x308D;&#12356;&#x304B;&#12406;&#12392;</trd>
        </trdgrp>
        <trd lng="uk">&#x43E;&c_b;&#x43B;&c_a;&c_d;&c_u;&#x43D;&#x43E;&#x43A;</trd>
  </drv>

    <drv mrk="arm.mal0isen0igi">
      <kap>mal<tld/>i,
      <var><kap>sen<tld/>igi</kap></var></kap>
      <snc mrk="arm.mal0isen0igi.MIL">
        <uzo tip="fak">MIL</uzo>
        <uzo tip="fak">POL</uzo>
	<dif>
	  Forigi la <tld/>ilojn de iu a&ubreve; io:
	  <ekz>
	    la demando pri &gcirc;enerala mal<tld/>ado
	    <klr>(mal<tld/>i&gcirc;o)</klr>;
	  </ekz>
          <ekz>
            la Azianoj sen<tld/>igis ilin sen kontra&ubreve;staro
            <fnt>
              <bib>Far2</bib>
              <lok>&Ccirc;apitro XIX</lok>
            </fnt>;
          </ekz>
          <ekz>
            fine ili estis venkitaj de la superforto, mal<tld/>itaj,
            metitaj en katenojn kaj trenitaj for
            <fnt>
              <aut>Michael Ende, trad. Wolfram Diestel</aut>
              <vrk>La Sen&ccirc;esa Rakonto</vrk>
              <lok>La Vidanta Mano</lok>
            </fnt>;
          </ekz>
          <ekz>
            <uzo tip="stl">FIG</uzo>
            li sin tenis tiel sincere, tiel modeste kaj afable, ke li
            sen<tld/>igis min
            <klr>(mi tute fidis lin, perdinte &ccirc;ian kontra&ubreve;staron,
            singardon)</klr>
            <fnt>
              <vrk><url ref="http://miresperanto.com/biblioteko/hhristovski/23.htm">23
              - miresperanto.com</url></vrk>
            </fnt>.
          </ekz>
	</dif>
        <trdgrp lng="zh">
    <trd>&#x89E3;&#38500;&#x6B66;&#x88C5; <pr>ji&#x11B;ch&#xFA;w&#x1D4;zhu&amacron;ng</pr></trd>,
    <trd>&#x89E3;&#38500;&#x6B66;&#x88DD; <pr>ji&#x11B;ch&#xFA;w&#x1D4;zhu&amacron;ng</pr></trd>,
    <trd>&#x653E;&#x4E0B;&#x6B66;&#22120; <pr>f&#xE0;ngxiaw&#x1D4;q&#xEC;</pr></trd>
  </trdgrp>
</snc>
      <trdgrp lng="be">
	<trd>&c_a;&c_b;&c_ja;&c_z;&c_z;&c_b;&c_r;&c_o;&c_ib;&c_c;&c_mol;</trd>,
