<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>arm</rad>/o
</kap>
<drv mrk="arm.0o">
  <kap><tld/>o<fnt>Z</fnt></kap>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="arm.0o"><tld lit="A"/>ilo</ref>:
      <ekz>
        ne servas larmo anstata&ubreve; <tld/>o<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <rim>
      Tia uzo de la vorto <tld/>o estas tre malofta, kvankam Zamenhofa.
    </rim>
  </snc>
  <trdgrp lng="de">
    <trd>Waffe</trd>, <trd>R&uuml;stzeug</trd>
  </trdgrp>
  <trdgrp
    lng="ru">
    <trd>&c_v;&c_o;&c_o;&c_r;&c_u;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_o;&c_r;&c_u;&c_zh;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="arm.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      &Ccirc;iuspeca ilo, uzebla por atake <ref cel="vund.0i">vundi</ref>,
      <ref cel="mort.0igi">mortigi</ref>, <ref cel="minac.0i">minaci</ref>
      pri tia atako, a&ubreve; <ref cel="defend.0i">defendi</ref>
      kontra&ubreve; &gcirc;i:
      <ekz>
        prenu &scirc;ildon kaj <tld/>ilojn, kaj stari&gcirc;u, por helpi
        min
        <fnt><aut>&Z;</aut> <vrk>&X;</vrk><lok>&Psa; 35:2</lok></fnt>;
      </ekz>
      <ekz>
        prenu do nun viajn <tld/>ilojn, vian sagujon kaj vian pafarkon,
        kaj iru sur la kampon kaj &ccirc;asu por mi &ccirc;asa&jcirc;on
        <fnt><aut>&Z;</aut> <vrk>&X;</vrk><lok>&Gen; 27:3</lok></fnt>;
      </ekz>
      <ekz>
        doni <tld/>ilojn en la manojn de niaj malamikoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        iris kun David &ccirc;irka&ubreve; kvarcent viroj, kaj ducent
        restis &ccirc;e la <tld/>ilaro
        <fnt><aut>&Z;</aut> <vrk>&X;</vrk><lok>&Sa1; 25:13</lok></fnt>;
      </ekz>
      <ekz>
        surmetu la tutan <ind><tld/>ilaro</ind>n de Dio, por ke vi povu
        kontra&ubreve;stari al la artifikoj de la diablo
        <fnt><vrk>&N;</vrk> <lok>&Efe; 6:11</lok></fnt>;
        <trd lng="fr">armement</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        aplombaj mensogoj estas iliaj preferataj <tld/>iloj.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="batal.0ilo">batalilo</ref>.
    </refgrp>
    <rim>
      <tld lit="A"/>iloj servas precipe por batali, sed anka&ubreve;
      por &ccirc;asi kaj fizike ekzerci&gcirc;i.
    </rim>
  </snc>
  <trd lng="fr">arme</trd>
</drv>

<drv mrk="arm.0ilaro">
  <kap><tld/>ilaro</kap>
  <snc>
    <dif>
      Tuto de la <tld/>iloj:
      <ekz>
        iris kun David &ccirc;irka&ubreve; kvarcent viroj, kaj ducent
        restis &ccirc;e la <tld/>ilaro
        <fnt><aut>&Z;</aut> <vrk>&X;</vrk><lok>&Sa1; 25:13</lok></fnt>;
      </ekz>
      <ekz>
        surmetu la tutan <tld/>ilaron de Dio, por ke vi povu
        kontra&ubreve;stari al la artifikoj de la diablo
        <fnt><vrk>&N;</vrk> <lok>&Efe; 6:11</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="arsena.0o.armilaro">arsenalo.</ref>
  </snc>
  <trd lng="fr">armement</trd>
</drv>

<drv mrk="arm.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Provizi per <tld/>iloj:
      <ekz>
        <tld/>i soldatojn per pafiloj;
      </ekz>
      <ekz>
        per fal&ccirc;iloj <tld/>itaj kamparanoj.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>bewaffnen</trd>, <trd>r&uuml;sten</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Plifortigi per diversaj rimedoj, subtraba&jcirc;o,
      la&ubreve;cela ilo, kc:
      <ekz>
        konstruo el <ref cel="beton.fer0osubferigita0oarmita0o"><tld/>ita
        betono</ref>;
      </ekz>
      <ekz>
        per ne<tld/>ita <klr>(nuda, senlorna)</klr> okulo<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>armieren</trd>, <trd>bewaffnen</trd>
    </trdgrp>
    <trdgrp
      lng="ru">
      <trd>&c_v;&c_o;&c_o;&c_r;&c_u;&c_zh;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_n;&c_a;&c_r;&c_ja;&c_zh;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">armer</trd>
</drv>

<drv mrk="arm.0ilejo">
  <kap><tld/>(il)ejo, <tld/>iltenejo</kap>
  <snc>
    <dif>
      Provizejo de <tld/>iloj:
      <ekz>
        &Hcirc;izkija [...] montris al ili sian trezorejon
        [...] kaj sian tutan armilejon
        <fnt><aut>&Z;</aut> <vrk>&X;</vrk><lok>&Re2; 21:13</lok></fnt>;
      </ekz>
      <ekz>
        via kolo estas kiel la turo de David, kiu estas konstruita
        kiel <tld/>iltenejo
        <fnt><aut>&Z;</aut> <vrk>&X;</vrk><lok>&Alt; 4:4</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="arsena.0o.armilejo">arsenalo.</ref>
  </snc>
  <trd lng="de">Waffenkammer</trd>
  <trd lng="fr">armurerie</trd>
  <trdgrp
    lng="ru">
    <trd>&c_a;&c_r;&c_s;&c_je;&c_n;&c_a;&c_l;</trd>,
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;
    &c_o;&c_r;&c_u;&c_zh;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="arm.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="arm.0ajxo.armilo">
    <dif>
      <ref tip="super" cel="arm.0ilo"><tld lit="A"/>ilo</ref>,
      portata por &scirc;irmi la korpon:
      <ekz>
        stari&gcirc;u en kaskoj, akrigu la lancojn, surmetu sur vin
        <tld/>a&jcirc;ojn
        <fnt><aut>&Z;</aut> <vrk>&X;</vrk><lok>&Jer; 46:4</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kiras.0o">kiraso</ref>,
      <ref cel="sxild.0o">&scirc;ildo</ref>.
    </refgrp>
    <trd lng="fr">armure</trd>
  </snc>
  <snc mrk="arm.0ajxo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Malmola kovra&jcirc;o, ekz-e metala, servanta por fortikigi
      kablon kaj protekti &gcirc;in kontra&ubreve; me&hcirc;anikaj
      dama&gcirc;oj.
    </dif>
    <trd lng="fr">armature<klr tip="ind"> (d'un c&acirc;ble)</klr></trd>
  </snc>
</drv>

<drv mrk="arm.mal0isen0igi">
  <kap>mal<tld/>i, sen<tld/>igi<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Forigi la <tld/>ilojn de iu a&ubreve; io:
      <ekz>
        la demando pri &gcirc;enerala mal<tld/>ado
        <klr>(mal<tld/>i&gcirc;o)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">entwaffnen</trd>
  <trd lng="fr">d&eacute;sarmer</trd>
  <trdgrp
    lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_o;&c_r;&c_u;&c_zh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_je;&c_z;&c_o;&c_r;&c_u;&c_zh;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="arm.sxirm0o">
  <kap>&scirc;irm<tld/>o</kap>
  <snc>
    <ref tip="dif" cel="arm.0ajxo.armilo"><tld/>a&jcirc;o.</ref>
  </snc>
</drv>
</art>
