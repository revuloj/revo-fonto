<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>arm</rad>/o
</kap>
<drv mrk="arm.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;iuspeca batalilo:
      <ekz>
        ne servas larmo anstata&ubreve; <tld/>o
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de"><trd>Waffe</trd>, <trd>R&uuml;stzeug</trd></trdgrp>
</drv>
<drv mrk="arm.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="arm.0o">Armo</ref>:
      <ekz>
        doni <tld/>ilojn en la manojn de niaj malamikoj<fnt>Z</fnt>.
      </ekz> 
      <ekz>
        <klr>(f)</klr> aplombaj mensogoj estas iliaj preferataj <tld/>iloj;
      </ekz>
    </dif>
  </snc>
</drv>
<drv mrk="arm.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1">
    <dif>
      Provizi per bataliloj por ataki a&ubreve; defendi:
      <ekz>
        <tld/>i soldatojn per pafiloj;
      </ekz>
      <ekz>
        per fal&ccirc;iloj <tld/>itaj kamparanoj;
      </ekz>
    </dif>
    <trdgrp lng="de"><trd>bewaffnen</trd>, <trd>r&uuml;sten</trd></trdgrp>
  </snc>
  <snc num="2">
    <dif>
      Plifortigi per diversaj rimedoj, subtraba&jcirc;o, 
      la&ubreve;cela ilo, kc:
      <ekz>
        konstruo el <tld/>ita <klr>(subferigita)</klr> cemento;
      </ekz>
      <ekz>
        per ne<tld/>ita <klr>(nuda, senlorna)</klr> 
        okulo<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de"><trd>armieren</trd>, <trd>bewaffnen</trd></trdgrp>
  </snc>
</drv>
<drv mrk="arm.0ilejo">
  <kap><tld/>(il)ejo</kap>
  <snc>
    <dif>
      Provizejo de <tld/>oj, arsenalo.
    </dif>
  </snc>
  <trd lng="de">Waffenkammer</trd>
</drv>
<drv mrk="arm.0ajxo">
  <kap><tld/>a&jcirc;o<fnt>Z</fnt>, &scirc;irm<tld/>o</kap>
  <snc>
    <dif>
      <tld/>o por sin &scirc;irmi, kiraso, &scirc;ildo.
    </dif>
  </snc>
</drv>
<drv mrk="arm.mal0isen0igi">
  <kap>mal<tld/>i, sen<tld/>igi<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Forigi la <tld/>ojn de...:
      <ekz>
        la demando pri &gcirc;enerala
        mal<tld/>ado <klr>(mal<tld/>i&gcirc;o)</klr>.
      </ekz>  
    </dif>
  </snc>
  <trd lng="de">entwaffnen</trd>
</drv>
</art>
<!--
$Log$
-->
</vortaro>