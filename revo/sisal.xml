<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sisal.xml,v 1.6 2023/08/21 13:29:35 revo Exp $">
<kap>
  <rad>sisal</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sisal.0o">
  <kap><tld/>o</kap>
  <snc mrk="sisal.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Solida fibro el la folioj de iuj <ref tip="vid" cel="agav.0o.BOT">agavoj</ref> el Meksiko, el
      kiu oni faras sakojn,
      &scirc;nurojn kaj similajn objektojn:
      <ekz>
        Tanzanio ekspluatas <klr>...</klr> kafon, kotonon kaj <tld/>on
        <fnt>
        <aut>Gbeglo K., By&iacute;elongo E.</aut>
        <vrk>Uea-prioritato Afriko-movado afrika: Tanzanio</vrk>
        <lok>soc.culture.esperanto, 2002-12-12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_y;&c_s;&c_a;&c_l;&c_mol;</trd>,
    <trd>&c_s;&c_y;&c_z;&c_a;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>sisal</trd>,
    <trd>vl&#xE1;kno z ag&#xE1;ve</trd>
  </trdgrp>
  <trd lng="fr">sisal</trd>
  <trd lng="pl">sizal</trd>
  <trd lng="sk">vl&#xE1;kno z ag&#xE1;ve</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&c_z;&c_a;&#x43B;</trd>,
    <trd>&c_s;&c_i;&c_z;&c_a;&#x43B;&#x44C;</trd>,
    <trd>&c_s;&c_i;&c_s;&c_a;&#x43B;&#x44C; (&c_v;&#x43E;&#x43B;&#x43E;&#x43A;&#x43D;&#x43E; &c_z; &#x43B;&c_i;&c_s;&c_t;&#x43A;&c_ib;&c_v;
    &c_a;&c_gu;&c_a;&c_v;&c_i;)</trd>
  </trdgrp>
</drv>
</art>

