<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Moz</rad>/o
</kap>
<drv mrk="moz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="river.0o">Rivero</ref>, fluanta el
      <ref tip="malprt" cel="franc.F0ujo">Francujo</ref>, tra
      <ref tip="malprt" cel="belg.B0ujo">Belgujo</ref> al
      <ref tip="malprt" cel="nederl.0o">Nederlando</ref>,
      kie &gcirc;i miksi&gcirc;as kun <ref tip="vid" cel="rejn.0o">Rejno</ref>:
      <ekz>
        <ctl>Senlime</ctl> estas la Esperanto-grupo en la regiono
        <tld/>o-Rejno.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">Meuse</trd>
  <trd lng="es">Mosa</trd>
  <trd lng="de">Maas</trd>
  <trd lng="fr">Meuse</trd>
  <trd lng="it">Mosa</trd>
  <trd lng="nl">Maas</trd>
  <trd lng="pt">Mosa</trd>
  <trd lng="pl">Moza</trd>
  <trdgrp lng="ru">
    <trd>&c_M;&c_a;&c_a;&c_s;</trd>,
    <trd>&c_M;&c_jo;&c_z;</trd>
  </trdgrp>
</drv>
</art>
