<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>telekopi</rad>/i
</kap>
<drv mrk="telekopi.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">TEL</uzo>
  <snc>
    <ref tip="dif" cel="faks.0i">faksi</ref>
  </snc>
<trd lng="de">faxen</trd>
<trd lng="nl">faxen</trd>
</drv>
<drv mrk="telekopi.0o">
  <kap><tld/>o</kap>
  <snc>
    <ref tip="dif" cel="faks.0o">fakso</ref>
  </snc>
<trd lng="de">Fax</trd>
<trd lng="nl">fax</trd>
</drv>
<drv mrk="telekopi.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="faks.0ilo">Faksilo</ref>:
      <ekz>
        adreso de KompRo: Bornkamp 12, D-22043 Hamburg,
        telekopiilo: + 49 40 656 06 59
        <fnt>
        <vrk><url ref="&Monato;/002001.html">Esperanto sen peno</url></vrk>, 
        <bib>Monato</bib>, 
        <lok>n-ro 09/1993</lok>
        </fnt>
      </ekz>.
     </dif>
  </snc>
<trd lng="de">Faxger&auml;t</trd>
<trd lng="nl">faxtoestel</trd>
</drv>
</art>
