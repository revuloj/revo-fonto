<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: telekopi.xml,v 1.22 2023/10/21 13:02:26 revo Exp $">
<kap>
  <rad>telekopi</rad>/o
</kap>
<drv mrk="telekopi.0o">
  <kap><tld/>o</kap>
  <snc mrk="telekopi.0o.TEL">
    <dif>
      <ref tip="dif" cel="faks.0o">Fakso</ref>:
      <ekz>
        se vi deziras specialajn informojn, sendu <tld/>on <klr>[&#x2026;]</klr>
        al <klr>[&#x2026;]</klr> +42.2.290-225
        <fnt>
          <aut>Reinhard Foessmeier</aut>,
          <vrk>AIS-sesio en &Ccirc;e&hcirc;io</vrk>,
          <lok>soc.culture.esperanto, 1992-03-10</lok>
        </fnt>;
      </ekz>
      <ekz>
        malpli multekoste ol papera sendado estas <tld/>o kaj retmesa&gcirc;o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=233582">Gazetara komuniko</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la uzataj sistemoj estas telekso, <tld/>o, video-teksto kaj la elektronika po&scirc;to
        <fnt>
          <vrk><url ref="http://ais-sanmarino.org/acta/cxifrado/cxif.html">Cxifrado kaj gxia rolo en
          la "nova komunikado - AIS San-Marino</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_k;&c_s;</trd>,
    <trd>&c_f;&c_a;&c_k;&c_s;&c_a;&c_v;&c_a;&c_je;
  &c_p;&c_a;&c_v;&c_je;&c_d;&c_a;&c_m;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;
    </trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>fax</trd>,
    <trd>kopie</trd>
  </trdgrp>
  <trd lng="de">Fax</trd>
  <trdgrp lng="en">
    <trd>fax message</trd>,
    <trd>fax</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fax<klr tip="ind"> (t&eacute;l&eacute;copie)</klr></trd>,
    <trd>t&eacute;l&eacute;copie</trd>
  </trdgrp>
  <trd lng="hu">fax</trd>
  <trd lng="nl">fax</trd>
  <trd lng="pl">faks</trd>
  <trdgrp lng="pt">
    <trd>fax</trd>,
    <trd>telec&oacute;pia</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_a;&c_k;&c_s; <klr tip="amb">
  (&c_s;&c_o;&c_o;&c_b;&c_shch;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trdgrp lng="sk">
    <trd>fax</trd>,
    <trd>k&oacute;pia</trd>
  </trdgrp>
  <trd lng="uk">&c_f;&c_a;&c_k;&c_s; (&c_k;&c_o;&c_p;&c_ib;&c_ja;)</trd>
</drv>

<drv mrk="telekopi.0i">
  <kap><tld/>i</kap>
  <snc mrk="telekopi.0i.TEL">
    <uzo tip="fak">TEL</uzo>
    <dif>
      <ref tip="dif" cel="faks.0i">Faksi</ref>:
      <ekz>
        la gekamaradoj <klr>[&#x2026;]</klr> petas, &ccirc;u ni povus <tld/>i al
        la jenaj turkaj institucioj <klr>[&#x2026;]</klr>
        <fnt>
          <aut>V. Bespalov</aut>,
          <vrk>turkoj bezonas solidarecon</vrk>,
          <lok>soc.culture.esperanto, 2001-01-03</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[per]</klr> <nom>ISDN</nom>-telefonoj <klr>[&#x2026;]</klr> la abonulo ekzemple samtempe
        telefonas kaj <tld/>as
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=204563">ISDN-telefono</url></lok>
        </fnt>.
      </ekz>
   </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_k;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">faxovat</trd>
  <trd lng="de">faxen</trd>
  <trd lng="en">fax</trd>
  <trdgrp lng="fr">
    <trd>faxer</trd>,
    <trd>t&eacute;l&eacute;copier</trd>
  </trdgrp>
  <trd lng="hu">faxol</trd>
  <trd lng="ja">&#x30D5;&#x30A1;&#x30C3;&#x30AF;&#x30B9;&#x3067;&#x9001;&#x308B;  <pr>&#x3075;&#x3041;&#x3063;&#x304F;&#x3059;&#x3067;&#x304A;&#x304F;&#x308B;</pr></trd>
  <trd lng="nl">faxen</trd>
  <trdgrp lng="pl">
    <trd>faksowa&cacute;</trd>,
    <trd>wysy&lstroke;a&cacute; <ind>faks</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>passar um fax</trd>,
    <trd>teleccopiar</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_l;&c_a;&c_t;&c_mol;
  <ind>&c_f;&c_a;&c_k;&c_s;</ind></trd>
  <trd lng="sk">faxova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &c_p;&c_o; &c_f;&c_a;&c_k;&c_s;&c_u;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_m;. &c_f;&c_a;&c_k;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="telekopi.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="telekopi.0ilo.TEL">
    <uzo tip="fak">TEL</uzo>
    <dif>
      <ref tip="dif" cel="faks.0ilo">Faksilo</ref>:
      <ekz>
        Miko &scirc;ovis la artikolon en la <tld/>ilon
        <fnt>
          <aut>Don Harlow</aut>,
          <vrk>Re: adresoj de Usonaj lingvikistoj</vrk>,
          <lok>soc.culture.esperanto, 1993-09-07</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ilo + 49&#x202F;40&#x202F;656&#x202F;0659
       <fnt>
          
          <vrk><url ref="&Monato;/1993/A930902.php">Noticoj</url></vrk>
          <bib>Monato</bib>
          <lok>1993/09, p. 4</lok>
        </fnt>.
      </ekz>
   </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_k;&c_s;</trd>,
    <trd>&c_f;&c_a;&c_k;&c_s;&c_a;&c_v;&c_y;
    &c_a;&c_p;&c_a;&c_r;&c_a;&c_t;</trd>
  </trdgrp>
  <trd lng="cs">fax <klr>(p&rcaron;&iacute;stroj)</klr></trd>
  <trd lng="de">Faxger&auml;t</trd>
  <trdgrp lng="en">
    <trd>fax machine</trd>,
    <trd>fax</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fax<klr tip="ind"> (t&eacute;l&eacute;copieur)</klr></trd>,
    <trd>t&eacute;l&eacute;copieur</trd>
  </trdgrp>
  <trd lng="hu">faxk&eacute;sz&uuml;l&eacute;k</trd>
  <trd lng="ja">&#x30D5;&#x30A1;&#x30C3;&#x30AF;&#x30B9;&#x6A5F;  <pr>&#x30D5;&#x30A1;&#x30C3;&#x30AF;&#x30B9;&#x304D;</pr></trd>
  <trd lng="nl">faxtoestel</trd>
  <trd lng="pl">faks</trd>
  <trdgrp lng="pt">
    <trd>fax</trd>,
    <trd>aparelho de fax</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_a;&c_k;&c_s;
  <klr tip="amb">(&c_a;&c_p;&c_p;&c_a;&c_r;&c_a;&c_t;)</klr></trd>
  <trd lng="sk">fax <klr>(pr&iacute;stroj)</klr></trd>
  <trd lng="uk">&c_f;&c_a;&c_k;&c_s; (&c_a;&c_p;&c_a;&c_r;&c_a;&c_t;)</trd>
</drv>
</art>
