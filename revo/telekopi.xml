<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>telekopi</rad>/o
</kap>
<drv mrk="telekopi.0o">
  <kap><tld/>o</kap>
  <snc mrk="telekopi.0o.TEL">
    <ref tip="dif" cel="faks.0ajxo.TEL">faksa&jcirc;o</ref>
    <ekz>
      se vi deziras specialajn informojn, sendu <tld/>on <klr>...</klr> al <klr>...</klr> +42.2.290-225
      <fnt>
        <aut>Reinhard Foessmeier</aut>,
        <vrk>AIS-sesio en &Ccirc;e&hcirc;io</vrk>,
        <lok>soc.culture.esperanto, 1992-03-10</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_k;&c_s;</trd>,
    <trd>&c_f;&c_a;&c_k;&c_s;&c_a;&c_v;&c_a;&c_je; &c_p;&c_a;&c_v;&c_je;&c_d;&c_a;&c_m;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="de">Fax</trd>
  <trdgrp lng="fr">
    <trd>fax<klr tip="ind"> (t&eacute;l&eacute;copie)</klr></trd>,
    <trd>t&eacute;l&eacute;copie</trd>
  </trdgrp>
  <trd lng="hu">fax</trd>
  <trd lng="nl">fax</trd>
  <trdgrp lng="pt">
    <trd>fax</trd>,
    <trd>telec&oacute;pia</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_a;&c_k;&c_s; <klr tip="amb">(&c_s;&c_o;&c_o;&c_b;&c_shch;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
</drv>

<drv mrk="telekopi.0i">
  <kap><tld/>i</kap>
  <snc mrk="telekopi.0i.TEL">
    <uzo tip="fak">TEL</uzo>
    <ref tip="dif" cel="faks.0i">faksi</ref>
    <ekz>
      la gekamaradoj <klr>...</klr> petas, &ccirc;u ni povus <tld/>i al la jenaj turkaj institucioj
      <fnt>
        <aut>V. Bespalov</aut>,
        <vrk>turkoj bezonas solidarecon</vrk>,
        <lok>soc.culture.esperanto, 2001-01-03</lok>
      </fnt>...
  </ekz>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_k;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">faxen</trd>
  <trdgrp lng="fr">
    <trd>faxer</trd>,
    <trd>t&eacute;l&eacute;copier</trd>
  </trdgrp>
  <trd lng="hu">faxol</trd>
  <trd lng="nl">faxen</trd>
  <trdgrp lng="pt">
    <trd>passar um fax</trd>,
    <trd>teleccopiar</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_l;&c_a;&c_t;&c_mol; <ind>&c_f;&c_a;&c_k;&c_s;</ind></trd>
</drv>

<drv mrk="telekopi.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="telekopi.0ilo.TEL">
    <uzo tip="fak">TEL</uzo>
    <ref tip="dif" cel="faks.0ilo">faksilo</ref>
    <ekz>
      Miko &scirc;ovis la artikolon en la <tld/>ilon
      <fnt>
        <aut>Don Harlow</aut>,
        <vrk>Re: adresoj de Usonaj lingvikistoj</vrk>,
        <lok>soc.culture.esperanto, 1993-09-07</lok>
      </fnt>.
    </ekz>
    <ekz>
      adreso de KompRo: Bornkamp 12, D-22043 Hamburg,
      <tld/>ilo: + 49 40 656 06 59
      <fnt>
        <aut>-</aut>,
        <vrk><url ref="&Monato;/002001.html">Esperanto sen peno</url></vrk>,
        <bib>Monato</bib>,
        <lok>jaro 1993a, numero 9a</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_k;&c_s;</trd>,
    <trd>&c_f;&c_a;&c_k;&c_s;&c_a;&c_v;&c_y; &c_a;&c_p;&c_a;&c_r;&c_a;&c_t;</trd>
  </trdgrp>
  <trd lng="de">Faxger&auml;t</trd>
  <trdgrp lng="fr">
    <trd>fax<klr tip="ind"> (t&eacute;l&eacute;copieur)</klr></trd>,
    <trd>t&eacute;l&eacute;copieur</trd>
  </trdgrp>
  <trd lng="hu">faxk&eacute;sz&uuml;l&eacute;k</trd>
  <trd lng="nl">faxtoestel</trd>
  <trdgrp lng="pt">
    <trd>fax</trd>,
    <trd>aparelho de fax</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_a;&c_k;&c_s; <klr tip="amb">(&c_a;&c_p;&c_p;&c_a;&c_r;&c_a;&c_t;)</klr></trd>
</drv>
</art>
