<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rusul.xml,v 1.11 2024/04/14 06:28:51 revo Exp $">
<kap>
  <rad>rusul</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="rusul.0o">
  <kap><tld/>o</kap>
  <snc mrk="rusul.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Lamena <ref tip="lst" cel="fung.0o.fruktokorpo" lst="voko:fungoj">fungo</ref> kun kolora
      &ccirc;apelo, blanka &scirc;tipo, de
      kiu kelkaj 
      specioj estas man&gcirc;eblaj;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref>
       el familio <ref tip="malprt" cel="rusulac.0oj">rusulacoj</ref>
       (<trd lng="la">Russula</trd>):
      <ekz>
        <ctl>Esperantlingva nomenklaturo de la fungogenro <tld/>o</ctl>
        <fnt>
          <aut>J. Kavka</aut>,
          <vrk>Esperantlingva nomenklaturo de la fungogenro
          rusulo</vrk>,
          <lok>Scienca Revuo, 1987, numero 1a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj kun blua a&ubreve; roza koloro
        <fnt>
          <aut>A. Baranauskas, trad. P. &Ccaron;eliauskas</aut>,
          <vrk>La Arbaro de Anyk&scaron;&ccaron;iai</vrk>,
          <lok>Ryto varpas, 2003</lok>
        </fnt>.
      </ekz>
    </dif>
  <rim mrk="rusul.0o.RIM_SIN">
    Alia esperantigo: <em>rusolo</em>.
  </rim>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_r;&c_a;&c_je;&c_zh;&c_k;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>cualba</trd>,
    <trd>r&uacute;ssula</trd>
  </trdgrp>
  <trd lng="cs">holubinka</trd>
  <trd lng="de">T&auml;ubling</trd>
  <trd lng="fr">russule</trd>
  <trd lng="it">rossola</trd>
  <trd lng="pl">go&lstroke;&aogonek;bek  <klr>(grzyb)</klr></trd>
  <trd lng="sk">pl&#xE1;vka</trd>
  <trd lng="uk">&c_s;&c_i;&c_r;&#x43E;&c_ji;&c_zh;&#x43A;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x7D05;&#x83C7;&#x79D1; <pr>h&#xF3;ngg&#x16B;k&#x113;</pr></trd>,
    <trd>&#x7EA2;&#x83C7;&#x79D1; <pr>h&#xF3;ngg&#x16B;k&#x113;</pr></trd>
  </trdgrp>
</drv>
</art>

