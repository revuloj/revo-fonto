<drv mrk="veter.bel0o">
  <kap>
    bel<tld/>o
  </kap>
  <snc>
    <dif>
      Agrabla <tld/>o, suna, varma ktp:
      <ekz>
        portikoj, kies kolonoj dum bel<tld/>o spegulas sin en la akvo
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXXVII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Sch&ouml;nwetter</trd>,
    <trd>Sonnenschein</trd>
  </trdgrp>
</drv>

<drv mrk="veter.mis0o">
  <kap>mis<tld/>o, <var><kap>malbon<tld/>o</kap></var></kap>
  <snc mrk="veter.mis0o.MET">
    <tezrad fak="MET"/>
    <uzo tip="fak">MET</uzo>
    <dif>
      Malagrabla <tld/>o &mdash; malseka, &scirc;torma ktp:
      <ekz>
        ekstere fari&gcirc;is terura malbon<tld/>o, forte pluvegis
        <fnt>
          <bib>Fab1</bib>
          <lok>Malbonkonduta knabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        tipe ragnarssona kontrastigo, mis<tld/>o kaj sunbrilo rapide
        alternas en la sama tago
        <fnt><aut>Mauro Nervi</aut>
           <vrk>La&ubreve; dezerto retorika: Baldur Ragnarsson kaj la etiko
           de silento</vrk>
           <lok><url
           ref="http://www.liberafolio.org/arkivo/www.liberafolio.org/2007/baldureseo/?searchterm=Ragnarsson">Libera
           folio</url>, 2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_p;&c_a;&c_g;&c_a;&c_d;&c_z;&c_mol;</trd>,
    <trd>&c_n;&c_ja;&c_g;&c_o;&c_d;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Unwetter</trd>,
    <trd>Schlechtwetter</trd>
  </trdgrp>
  <trdgrp lng="en">
