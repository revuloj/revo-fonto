<drv mrk="veter.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="veter.0o.MET">
    <tezrad fak="MET"/>
    <uzo tip="fak">MET</uzo>
    <dif>
      Stato de la atmosfero:
      <ekz>
        hodia&ubreve; estas bela frosta <tld/>o, tial mi prenos miajn glitilojn kaj iros gliti
        <fnt>
          <bib>F</bib>
          <lok>&FE; 34</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la kota <tld/>o mia vesto forte malpuri&gcirc;is
        <fnt>
          <bib>F</bib>
          <lok>&FE; 39</lok>
        </fnt>;
      </ekz>
      <ekz>
        tendo por <klr>[&#x2026;]</klr> &scirc;irmi kontra&ubreve; malbona <tld/>o kaj pluvo
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 4:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        malseka <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Malbonkonduta knabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        terure malbona <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        serena frosta <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        estis belega printempa <tld/>o kun floroj kaj verda&jcirc;o
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        en pluva <tld/>o mi &ccirc;iam estas hejme
        <fnt>
          <bib>Fab2</bib>
          <lok>La ombro</lok>
        </fnt>!
      </ekz>
      <ekz>
        la <tld/>o estis a&ccirc;a, malseka kaj tempesta &dash; gripa <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>transir<tld/>o</ind>, &ccirc;u sekvos vintro &ndash; a&ubreve; printempo
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 2 (100)</lok>
        </fnt>?
        <trd lng="de">&Uuml;bergangswetter</trd>
      </ekz>
      <ekz>
        en trankvila <tld/>o &ccirc;iu remas sen dan&gcirc;ero
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>aprila <tld/>o, trompa aero</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="de">April, April, der wei&szlig; nicht, was er will</trd>
        <trd lng="fr">en avril ne te d&eacute;couvre pas d'un fil</trd>
        <trd lng="hu">tavasz &ndash; ravasz</trd>
      </ekz>
      <ekz>
        post <tld/>o malbela lumas suno plej hela
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="meteor2.0o">meteorologio</ref>,
      <ref cel="scienc.veter0o">veterscienco</ref>
    </refgrp>
  </snc>
