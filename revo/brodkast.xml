<art mrk="$Id: brodkast.xml,v 1.12 2025/03/28 16:17:59 revo Exp $">
<kap>
  <rad>brodkast</rad>/i <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="brodkast.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="brodkast.0i.TEL">
    <uzo tip="fak">RAD</uzo>
    <uzo tip="fak">TEL</uzo>
    <dif>
      <ref tip="super" cel="send.el0i.TEL">Elsendi</ref> per ondoj:
      <ekz>
        tio estas kanto tre konata en Anglujo: la B.B.C. <tld/>is
        &gcirc;in en
        Esperanto en 1920
        <fnt>
        <aut>M. C. Butler</aut>,
        <vrk>Pri versfarado kaj kanttraduko</vrk>,
        <lok><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr71/kanttraduko.html">Nica
        Literatura Revuo, 1961, 7:1 (37)</url>, p. 31a-35a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">senden<klr tip="amb"> (Rundfunk)</klr></trd>
    <trd lng="fr">diffuser<klr tip="amb">
    (radiot&eacute;l&eacute;com.)</klr></trd>
    <trd lng="hu">sug&aacute;roz<klr tip="amb">
    (m&udblac;sort)</klr></trd>
    <trdgrp lng="pl">
      <trd>nadawa&cacute;<klr> (o radiu)</klr></trd>,
      <trd>rozsy&lstroke;a&cacute;<klr> (wiadomo&sacute;ci,
      informacje)</klr></trd>,
      <trd>rozsiewa&cacute;<klr> (o radiostacji)</klr></trd>
    </trdgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>dissendi</ke>(per radio, televido).
  </rim>

  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_mol;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_p;&c_r;&c_a;&c_z; &c_e;&c_t;&c_e;&c_r;)</klr></trd>
  </trdgrp>
  <trd lng="cs">vys&#237;lat <klr>(r&#225;dio)</klr></trd>
  <trd lng="he">&#1500;&#1513;&#1491;&#1512;</trd>
  <trd lng="sk">vysiela&tcaron; <klr>(r&#225;dio)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_r;&c_a;&#1085;&c_s;&#1083;&#1102;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#1087;&c_je;&c_r;&c_je;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &#1087;&#1086; &c_r;&c_a;&c_d;&c_ib;&#1086;</trd>
  </trdgrp>
</drv>
</art>
