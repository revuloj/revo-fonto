<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>1</ofc>
      <rad>arme</rad>/o
    </kap>
    <drv mrk="arme.0o">
      <kap><tld/>o</kap>
      <snc mrk="arme.0o.sxtata">
        <uzo tip="fak">POL</uzo>
        <dif>
          La tuta organizita milita forto de 
          <ref tip="malprt" cel="sxtat.0o">&scirc;tato</ref>: la
          <ref tip="prt" cel="soldat.0o">soldatoj</ref>,
          <ref tip="prt" cel="oficir.sub0o">suboficiroj</ref> kaj
          <ref tip="prt" cel="oficir.0o">oficiroj</ref>,
          <ref tip="prt" cel="arm.0ilo">armiloj</ref>.
        </dif>
        <ref tip="vid" cel="milit.0istaro">militistaro</ref>
        <rim>
          En multaj landoj oni distingas inter la (tera) <tld/>o kaj
          la <ref cel="flot.0o.MIL">floto</ref>.  PIV tamen rigardas
          la floton parto de <tld/>o:
          <ekz>
            <ind>aer<tld/>o</ind>,
            <trd lng="ru">&c_v;&c_o;&c_z;&c_d;&c_u;&c_sh;&c_n;&c_y;&c_je; &c_v;&c_o;&c_j;&c_s;&c_k;&c_a;</trd>
          </ekz><ekz>
            <ind>ter<tld/>o</ind>,
            <trd lng="ru">&c_s;&c_u;&c_h;&c_o;&c_p;&c_u;&c_t;&c_n;&c_y;&c_je; &c_v;&c_o;&c_j;&c_s;&c_k;&c_a;</trd>
          </ekz><ekz>
            mar<tld/>o.
          </ekz>
        </rim>
        <trd lng="ru">&c_v;&c_o;&c_o;&c_r;&c_u;&c_zh;&c_jo;&c_n;&c_n;&c_y;&c_je; &c_s;&c_i;&c_l;&c_y;</trd>
      </snc>
      <snc mrk="arme.0o.unuo">
        <uzo tip="fak">MIL</uzo>
        <dif>
          Parto de <tld/>o,<sncref ref="arme.0o.sxtata"/>, konsistanta
          el <ref cel="korpus.0o.MIL">korpusoj</ref> a&ubreve; malpli grandaj
          trupunuoj, komandata de unu &ccirc;efgeneralo.
        </dif>
        <trd lng="ru">&c_a;&c_r;&c_m;&c_i;&c_ja;</trd>
      </snc>
      <snc mrk="arme.0o.FIG">
        <uzo tip="stl">FIG</uzo><dif>
          Multnombra kolekto da homoj a&ubreve; bestoj:
          <ekz>
            <tld/>o de la laboristoj, da akridoj, da petantoj<fnt>Z</fnt>.
          </ekz>
        </dif>
        <trd lng="hu">egy <ind>sereg</ind></trd>
      </snc>
      <trd lng="fr">arm&eacute;e</trd>
      <trd lng="hu">hadsereg</trd>
      <trd lng="nl">leger</trd>
      <trdgrp lng="ru">
        <trd>&c_a;&c_r;&c_m;&c_i;&c_ja;</trd>,
        <trd>&c_v;&c_o;&c_j;&c_s;&c_k;&c_o;</trd>
      </trdgrp>
    </drv>
    <drv mrk="arme.sav0o">
      <kap>Sav<tld/>o</kap>
      <snc>
        <dif>
          Internacia religia organiza&jcirc;o bonfarada,
          fondita en 1878 de W. Booth.
        </dif>
      </snc>
      <trd lng="fr">Arm&eacute;e du Salut</trd>
      <trdgrp lng="nl">
        <trd>Leger des Heils</trd>,
        <trd>Heilsleger</trd>
      </trdgrp>
      <trd lng="ru">&c_A;&c_r;&c_m;&c_i;&c_ja; &c_s;&c_p;&c_a;&c_s;&c_je;&c_n;&c_i;&c_ja;</trd>
    </drv>
  </art>
