<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: leksem.xml,v 1.5 2023/04/11 16:10:37 revo Exp $">
<kap>
  <rad>leksem</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="leksem.0o">
  <kap><tld/>o</kap>
  <snc mrk="leksem.0o.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      Elementa ero
      (<ref tip="super" cel="morfem.0o.GRA">morfemo</ref>)
      de lingvo, kiu havas sencon:
      <ekz>
        ofteco de <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="&Monato;/2008/009278.php"
              >33 jaroj da perfektigo</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj tri partoj enhavas <tld/>ojn por signifoj
        kaj markilojn por gramatikaj funkcioj
        <fnt>
          <aut>Liu Haitao</aut>
          <vrk><url ref="http://www.lingviko.net/kod.pdf">
          Kio estas KOD? Kiel &gcirc;i funkciu?</url></vrk>
          <lok>Informilo por interlingvistoj, 2006(2):2-5</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi trovis misuzon de E-<tld/>o en &ccirc;iu tria linio
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 7 (69)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="leksik2.0o">leksiko</ref>
    </refgrp>
  </snc>
  <trd lng="de">Lexem</trd>
  <trd lng="fr">lex&egrave;me</trd>
  <trd lng="pl">leksem</trd>     
  <trd lng="ro">lexem</trd>
  <trd lng="uk">&c_l;&c_je;&c_k;&c_s;&c_je;&c_m;&c_a;</trd>
</drv>
</art>

