<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>dezajn</rad>/i  <fnt><bib>SPIV</bib></fnt>
</kap>
<drv mrk="dezajn.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <ref tip="dif" cel="dizajn.0i">dizajni</ref>
  <ekz>
    Guillem Sevilla  <klr>...</klr> estas grafikisto; li
    <tld/>as librojn, retejojn, logotipojn
    <fnt>
      <aut>Kataluna Esperanto-Asocio</aut>,
      <vrk><url
      ref="http://www.esperanto.cat/kongresoj/2011/kongreslibro_w.pdf">
      Kongreslibro</url></vrk>,
      <lok>2011</lok>
    </fnt>.
  </ekz>
  <trd lng="sk">vytvárať dizajn</trd>
  <trd lng="cs">vytvářet design</trd>
</drv>

<drv mrk="dezajn.0o">
  <kap><tld/>o</kap>
  <ref tip="dif" cel="dizajn.0o">dizajno</ref>
  <ekz>
    la <tld/>o de bastonvalizo multe &scirc;an&gcirc;i&gcirc;is ene de &gcirc;ia dekjara
    historio
    <fnt>
      <aut>&Jcirc;. Amis</aut>,
      <vrk>Mia malkovro de Japanujo</vrk>,
      <lok><url
      ref="http://bulteno.esperanto-usa.org/2013/1/40-vojagxe-tra-la-mondo.html">
      Usona Esperantisto, 2013:1</url></lok>
    </fnt>.
  </ekz>
  <trdgrp lng="sk">
    <trd>náčrt</trd>,
    <trd>vzhľad</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>design <klr>(dyzajn)</klr></trd>,
    <trd>náčrt</trd>,
    <trd>vzhled</trd>
  </trdgrp>
</drv>

<drv mrk="dezajn.0isto">
  <kap><tld/>isto</kap>
  <ref tip="dif" cel="dizajn.0isto">dizajnisto</ref>
  <ekz>
    du londonaj <tld/>istoj realigis revolucian lampon funkciantan per
    gravito
    <fnt>
      <aut>G. P. Savio</aut>,
      <vrk><url
      ref="http://esperantomovado.blogspot.fr/2013/04/lumigi-per-gravito.html">
      Lumigi per gravito</url></vrk>,
      <lok>Esperanta Retradio, 2013-04-08</lok>
    </fnt>.
  </ekz>
</drv>
</art>

