  </refgrp>
  <trd lng="fr">aigrette<klr tip="ind"> (touffe de plumes)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_o;&c_h;&c_o;&c_l;&c_o;&c_k;</trd>,
    <trd>&c_s;&c_u;&c_l;&c_t;&c_a;&c_n;</trd>
