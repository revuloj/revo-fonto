<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: egret.xml,v 1.20 2020/09/10 22:32:16 revo Exp $">
<kap>
  <rad>egret</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="egret.0o">
  <kap><tld/>o</kap>
  <snc mrk="egret.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Surkapa tufo de plumoj de iuj birdoj,
      <ref tip="sin" cel="tuf.plum0o">plumtufo</ref>,
      <ref tip="vid" cel="krest.0o.ZOO_pluma">kresto<sncref/></ref>:
      <ekz>
        <tld/>ardeo.
      </ekz>
    </dif>
  </snc>
  <snc mrk="egret.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Ornama tufo de plumoj sur kapvesto;
      <ref tip="vid" cel="krest.0o.TEKS">kresto<sncref/></ref>.
    </dif>
  </snc>
  <trd lng="ca">plomall</trd>
  <trdgrp lng="cs">
    <trd>p&eacute;&rcaron;ov&yacute; chochol</trd>,
    <trd>p&eacute;&rcaron;ov&yacute; h&rcaron;eben</trd>
  </trdgrp>
  <trd lng="de">Federkrone</trd>
  <trdgrp lng="en">
    <trd>crest</trd>
  </trdgrp>
  <trd lng="fr">aigrette<klr tip="ind"> (touffe de plumes)</klr></trd>
  <trd lng="hu">b&oacute;bita</trd>
  <trd lng="nl">aigrette</trd>
  <trdgrp lng="pl">
    <trd>pi&oacute;ropusz</trd>,
    <trd>czub</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>penacho</trd>,
    <trd>topete <klr tip="amb">(de ave)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_h;&c_o;&c_h;&c_o;&c_l;&c_o;&c_k;<klr tip="ind"> (&c_i;&c_z;
    &c_p;&c_je;&c_r;&c_mol;&c_je;&c_v;)</klr></trd>,
    <trd>&c_s;&c_u;&c_l;&c_t;&c_a;&c_n; <klr tip="amb">(&c_i;&c_z;
    &c_p;&c_je;&c_r;&c_mol;&c_je;&c_v;)</klr></trd>
  </trdgrp>
  <trd lng="sk">iskrenie el. pri vysokom nap&auml;t&iacute;</trd>
  <trdgrp lng="cs">
    <trd>chochol</trd>,
    <trd>h&rcaron;eben</trd>
  </trdgrp>
</drv>
</art>
