<drv mrk="kub.0ismo">
   <kap><tld/>ismo</kap>
   <uzo tip="fak">BELA</uzo>
   <snc mrk="kub.0ismo.stilo">
     <dif>
       Skulpta kaj pentra <ref tip="super" cel="stil.0o.BELA.loka">stilo</ref> franc-devena, 
       prospera komence de la 20a jarcento (proksimume 1910&ndash;1930) precipe en Francujo kaj
       &Ccirc;e&hcirc;ujo, influita de afrikaj arta&jcirc;oj, montranta simpligitajn,
       geometriajn formojn de objektoj, miksanta plurajn vid-punktojn al ili,
       algluanta al la verkoj materialojn el la bildigita&jcirc;oj (gazetfoliojn kaj
       aliajn):
      <ekz>
        inspirita de <tld/>ismo li e&ccirc; mane fabrikis 13 violonojn
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="https://www.monato.be/2014/011335.php"
              >Pentristo de la homo kaj de la naturo</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        e&ubreve;ropaj kritikistoj emas paroli pri realismo a&ubreve; <tld/>ismo
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="https://www.monato.be/2006/008835.php"
              >500 &ccirc;efverkoj en Monako</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_k;&c_u;&c_b;&c_ib;&c_z;&c_m;</trd>
  <trd lng="cs">kubizmus</trd>
  <trd lng="de">Kubismus</trd>
  <trd lng="en">cubism</trd>
  <trd lng="fa">&f_k;&a_w;&a_b;&f_y;&a_s;&a_m;</trd>
  <trd lng="fr">cubisme</trd>
  <trd lng="hu">kubizmus</trd>
  <trd lng="id">kubisme</trd>
  <trd lng="nl">kubisme</trd>
  <trd lng="pl">kubizm</trd>     
  <trd lng="ro">cubism</trd>
  <trd lng="ru">&c_k;&c_u;&c_b;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">kubizmus</trd>
  <trd lng="sv">kubism</trd>
</drv>

<drv mrk="kub.jxet0o">
  <kap>&jcirc;et<tld/>o</kap>
  <snc mrk="kub.jxet0o.SPO">
  <uzo tip="fak">SPO</uzo>
    <dif>
      <tld lit="K"/>o uzata en iuj hazardludoj:
      <ekz>
        iu metis sur la tero grandan nigran &jcirc;et<tld/>on
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        duono de rompita &jcirc;et<tld/>o de la feli&ccirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>W&uuml;rfel</trd>,
      <trd>Spielw&uuml;rfel</trd>
    </trdgrp>
    <trd lng="en">die <klr>(1 die, 2 dice)</klr></trd>
    <trd lng="es">dado <klr>(de juego)</klr></trd>
    <trd lng="fr">d&eacute;</trd>
    <trd lng="hu">dob&oacute;kocka</trd>
    <trd lng="id">dadu</trd>
    <trd lng="it">dado<klr> (da gioco)</klr></trd>
    <trd lng="nl">dobbelsteen</trd>
    <trd lng="pl">kostka<klr> do gry</klr></trd>
    <trd lng="ru">&c_i;&c_g;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
  </snc>
</drv>

<drv mrk="kub.0forma">
  <kap><tld/>forma</kap>
  <snc>
    <dif>
      Havanta formon de <tld/>o:
      <ekz>
        li volas eskapi el tiu &ccirc;i stranga karcero <tld/>forma
        <fnt>
          <bib>Monato</bib>
          <aut>Marteno Ecott</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006838.html"
              >Kinarto: Inkubo en kubo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>forma satelito, malpli ol 500 gramojn peza
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="https://www.monato.be/2014/011232.php"
              >Kosma kubo</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_b;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="de">w&uuml;rfelf&ouml;rmig</trd>
  <trdgrp lng="en">
     <trd>cubic</trd>,
     <trd>cubical</trd>
   </trdgrp>
  <trdgrp lng="fa">
     <trd>&a_m;&f_k;&a_ayn;&a_b;&f_y;&zwnj;&a_sh;&f_k;&a_l;</trd>,
     <trd>&a_m;&f_k;&a_ayn;&a_b;&f_y;</trd>
   </trdgrp>
  <trd lng="fr">cubique</trd>
  <trd lng="nl">kubusvormig</trd>
  <trd lng="pl">w kszta&lstroke;cie <ind>kostki</ind></trd>     
  <trd lng="ro">sub form&abreve; de <ind>cuburi</ind></trd>
  <trd lng="ru">&c_k;&c_u;&c_b;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">kockovit&yacute;</trd>
  <trd lng="sv">kubformad</trd>
</drv>
</art>
