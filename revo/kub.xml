<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>8</ofc>
   <rad>kub</rad>/o
</kap>
<drv mrk="kub.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="kub.0o.figuro">
     <uzo tip="fak">MAT</uzo>
     <fnt><bib>MatTerm</bib><lok>p. 30</lok></fnt>
     <dif>
       <ref tip="super" cel="edr.n0o">Sesedro</ref>, kiun limas ses
       <ref cel="kvadra1.0o.figuro">kvadratoj</ref>; alidire
       <ref cel="regul.0a.pluredro">regula</ref> sesedro.
     </dif>
     <trd lng="be">&c_k;&c_u;&c_b;</trd>
     <trd lng="de">W&uuml;rfel</trd>
     <trd lng="en">cube</trd>
     <trd lng="hu">kocka</trd>
     <trdgrp lng="pl">
       <trd>sze&sacute;cian<klr tip="ind"> (wielo&sacute;cian
foremny)</klr></trd>,
       <trd>kostka</trd>
     </trdgrp>
     <trd lng="ru">&c_k;&c_u;&c_b;</trd>
   </snc>
   <snc>
     <dif>
       Malgranda <tld/>o el eburo, ligno ktp, signita sur &ccirc;iu
       faco per aparta nombro, de unu &gcirc;is ses, kaj servanta por
diversaj
       ludoj; &jcirc;et<tld/>o, lud<tld/>o:
       <ekz>
         la <tld/>oj jam falis, jam estas &jcirc;etitaj
         <klr>(malebli&gcirc;is nun &ccirc;ia &scirc;an&gcirc;i&gcirc;o;
         <refgrp tip="vid">
           <ref cel="pont.0o">ponto</ref>,
           <ref cel="pas.trans0i">transpasi</ref>
         </refgrp>)</klr>.
       </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>&c_k;&c_u;&c_b;</trd>,
       <trd>&c_k;&c_u;&c_b;&c_ib;&c_k;</trd>
     </trdgrp>
     <trd lng="hu">dob&oacute;kocka</trd>
     <trd lng="ru">&c_k;&c_u;&c_b;&c_i;&c_k;</trd>
   </snc>
   <snc mrk="kub.0o.algebra">
     <uzo tip="fak">MAT</uzo>
     <fnt><bib>MatTerm</bib><lok>p. 10</lok></fnt>
     <dif>
       La tria <ref tip="super"
cel="potenc.0o.per_entjero">potenco</ref> de
       nombro; produto de tiu nombro per &gcirc;ia
       <ref cel="kvadra1.0o.algebra">kvadrato</ref>.
     </dif>
     <trdgrp lng="be">
       <trd>&c_k;&c_u;&c_b;</trd>,
       <trd>&c_t;&c_r;&c_e;&c_c;&c_ja;&c_ja;
&c_s;&c_t;&c_u;&c_p;&c_je;&c_n;&c_mol;</trd>
     </trdgrp>
     <trd lng="de">Kubus</trd>
     <trd lng="en">cube</trd>
     <trd lng="hu">k&ouml;b</trd>
     <trdgrp lng="pl">
       <trd>sze&sacute;cian<klr tip="ind"> (trzecia
pot&eogonek;ga)</klr></trd>,
       <trd>trzecia pot&eogonek;ga<klr tip="ind">
(liczby)</klr></trd></trdgrp>
     <trdgrp lng="ru">
       <trd>&c_k;&c_u;&c_b;</trd>,
       <trd>&c_t;&c_r;&c_je;&c_t;&c_mol;&c_ja;
&c_s;&c_t;&c_je;&c_p;&c_je;&c_n;&c_mol;</trd>
     </trdgrp>
   </snc>
   <trd lng="fr">cube</trd>
</drv>

<drv mrk="kub.0a">
   <kap><tld/>a</kap>
   <fnt><bib>PIV1</bib></fnt>
   <snc>
     <dif>
       Rilata al <tld/>o:
       <ekz>
         <tld/>a skatolo <klr>(<ref
         cel="kub.0forma"><tld/>forma</ref>)</klr>;
       </ekz>
       <ekz>
         <ref cel="metr.kuba0o"><tld/>a metro</ref>;
       </ekz>
       <ekz>
         <uzo tip="fak">MAT</uzo>
         <tld/>a polinomo <klr>(triagrada)</klr>;
       </ekz>
       <ekz>
         <uzo tip="fak">MAT</uzo>
         <tld/>a <ref cel="radik.0o.per">radiko</ref>.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_k;&c_u;&c_b;&c_ib;&c_ch;&c_n;&c_y;</trd>
   <trd lng="de">kubisch</trd>
   <trdgrp lng="en">
     <trd>cubic</trd>,
     <trd>cubical</trd>
   </trdgrp>
   <trd lng="fr">cubique</trd>
   <trdgrp lng="hu">
     <trd>k&ouml;b-</trd>,
     <trd>kocka alak&uacute;</trd></trdgrp>
   <trdgrp lng="pl">
     <trd>sze&sacute;cienny</trd>,
     <trd>kubiczny</trd>,
     <trd>trzeciego stopnia</trd>,
   </trdgrp>
   <trd lng="ru">&c_k;&c_u;&c_b;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="kub.0isma">
   <kap><tld/>isma</kap>
   <snc>
     <dif>
       Konforma al <ref tip="vid" cel="kub.0ismo"><tld/>ismo</ref>.
     </dif>
   </snc>
   <trd lng="be">&c_k;&c_u;&c_b;&c_ib;&c_s;&c_c;&c_k;&c_ib;</trd>
   <trd lng="de">kubistisch</trd>
   <trd lng="hu">kubista</trd>
   <trdgrp lng="ru">
     <trd>&c_k;&c_u;&c_b;&c_i;&c_s;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>,
     <trd>&c_k;&c_u;&c_b;&c_i;&c_s;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
   </trdgrp>
</drv>

<drv mrk="kub.0ismo">
   <kap><tld/>ismo</kap>
   <uzo tip="fak">BELA</uzo>
   <snc>
     <dif>
       Skulpta kaj pentra stilo franc-devena, prospera komence de la 20a
jarcento
       (proksimume 1910&ndash;1930) precipe en Francujo kaj
&Ccirc;e&hcirc;ujo,
       influita de afrikaj arta&jcirc;oj, montranta simpligitajn,
geometriajn
       formojn de objektoj, miksanta plurajn vid-punktojn al ili, algluanta
al
       la verkoj materialojn el la bildigita&jcirc;oj (gazetfoliojn kaj
aliajn).
     </dif>
   </snc>
   <trd lng="be">&c_k;&c_u;&c_b;&c_ib;&c_z;&c_m;</trd>
   <trd lng="de">Kubismus</trd>
   <trd lng="fr">cubisme</trd>
   <trd lng="hu">kubizmus</trd>
   <trd lng="ru">&c_k;&c_u;&c_b;&c_i;&c_z;&c_m;</trd>
</drv>

<drv mrk="kub.0forma">
   <kap><tld/>forma</kap>
   <snc>
     <dif>
       Havanta formon de <tld/>o.
     </dif>
   </snc>
   <trd lng="be">&c_k;&c_u;&c_b;&c_ib;&c_ch;&c_n;&c_y;</trd>
   <trd lng="de">w&uuml;rfelf&ouml;rmig</trd>
   <trd lng="fr">cubique</trd>
   <trd lng="ru">&c_k;&c_u;&c_b;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
