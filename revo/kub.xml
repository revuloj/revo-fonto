<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kub.xml,v 1.50 2023/12/03 09:59:02 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>kub</rad>/o
</kap>
<drv mrk="kub.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="kub.0o.figuro">
     <uzo tip="fak">MAT</uzo>
     <fnt><bib>MatTerm</bib><lok>p. 30</lok></fnt>
     <dif>
       <ref tip="super" cel="edr.n0o">Sesedro</ref>, kiun limas ses
       <ref cel="kvadra1.0o.figuro">kvadratoj</ref>; alidire
       <ref cel="regul.0a.pluredro">regula</ref> sesedro:
      <ekz>
        la domoj <klr>[&#x2026;]</klr> similis blankajn <tld/>ojn, truitajn en la lokoj, kie
        estis la fenestroj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e &ccirc;iu faco de la <tld/>o estas plato, mon&scirc;rankaspekta
        <fnt>
       <aut>Marteno Ecott</aut>
          <vrk><url ref="&Monato;/2000/006838.html">Inkubo en kubo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 17</lok>
        </fnt>.
      </ekz>
     </dif>
     <trd lng="be">&c_k;&c_u;&c_b;</trd>
     <trd lng="en">cube</trd>
     <trd lng="es">cubo</trd>
     <trd lng="fa">&a_m;&f_k;&a_ayn;&a_b;</trd>
     <trd lng="hu">kocka</trd>
     <trd lng="id">kubus</trd>
     <trd lng="pl">sze&sacute;cian <klr tip="ind">(wielo&sacute;cian
     foremny)</klr></trd>
     <trd lng="nl">kubus</trd>
     <trd lng="pt">cubo</trd>
     <trd lng="ro">cub</trd>
     <trd lng="ru">&c_k;&c_u;&c_b;</trd>
     <trd lng="sv">kub</trd>
   </snc>
   <snc mrk="kub.0o.ludilo">
    <dif>
      Tia<ref tip="lst" lst="voko:formoj" cel="form.0o.aspekto">form</ref>a
      objekto ofte uzata en ludoj; <ref cel="kub.jxet0o">&jcirc;et<tld/>o</ref>, lud<tld/>o:
      <ekz>
         la <tld/>oj jam falis, jam estas &jcirc;etitaj
         <klr>(malebli&gcirc;is nun &ccirc;ia &scirc;an&gcirc;i&gcirc;o; kp
         <refgrp tip="vid">
           <ref cel="pont.0o">ponto</ref>,
           <ref cel="pas.trans0i">transpasi</ref> la
           <ref cel="rubikon.0o">Rubikono</ref>n</refgrp>)</klr>;
      </ekz>
      <ekz>
        sur la ses flankoj de &ccirc;iu <tld/>o estis literoj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Urbo de Imperiestroj</lok>
        </fnt>;
      </ekz>
      <ekz>
        komparebla kun la hungara freneziga ludilo, <ind>Rubika <tld/>o</ind>
        <fnt>
       <aut>Marteno Ecott</aut>
          <vrk><url ref="&Monato;/2000/006838.html">Inkubo en kubo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 17</lok>
        </fnt>.
        <trdgrp lng="de">
          <trd>Zauberw&uuml;rfel</trd>,
          <trd>Rubiks W&uuml;rfel<klr> [rare uzata]</klr></trd>
        </trdgrp>
        <trd lng="en">Rubik's Cube</trd>
        <trdgrp lng="it">
          <trd>cubo di Rubik</trd>,
          <trd>cubo magico</trd>
        </trdgrp>
        <trd lng="id">kubus Rubik</trd>
        <trd lng="ru">&c_k;&c_u;&c_b;&c_i;&c_k; &c_R;&c_u;&c_b;&c_i;&c_k;&c_a;</trd>
        <trd lng="tr">Rubik K&uuml;p&uuml;</trd>
      </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>&c_k;&c_u;&c_b;</trd>,
       <trd>&c_k;&c_u;&c_b;&c_ib;&c_k;</trd>
     </trdgrp>
     <trdgrp lng="en">
       <trd>die<klr tip="amb"> (gaming)</klr></trd>,
       <trd>dice</trd>
     </trdgrp>
     <trd lng="es">dado</trd>
     <trd lng="fa">&a_t;&a_A;&a_s;<klr tip="ind"> (&a_m;&a_h;&a_r;&a_h;&a_hamza_sure;
     &a_b;&a_A;&a_z;&f_y;)</klr></trd>
     <trd lng="hu">dob&oacute;kocka</trd>
     <trd lng="id">dadu</trd>
     <trdgrp lng="nl">
       <trd>teerling</trd>,
       <trd>dobbelsteen</trd>
     </trdgrp>
     <trd lng="pl">kostka</trd>
     <trd lng="ro">zar</trd>
     <trd lng="ru">&c_k;&c_u;&c_b;&c_i;&c_k;</trd>
     <trd lng="sv">t&auml;rning</trd>
   </snc>
   <snc mrk="kub.0o.algebra">
     <uzo tip="fak">MAT</uzo>
     <fnt><bib>MatTerm</bib><lok>p. 10</lok></fnt>
     <dif>
       La tria <ref tip="super" cel="potenc.0o.per_entjero">potenco</ref> de
       nombro; produto de tiu nombro per &gcirc;ia
       <ref cel="kvadra1.0o.algebra">kvadrato</ref>:
        <ekz>
          se <frm am="x"><k>x</k></frm> estas pozitiva reela nombro,
          la valoro de <frm am="x^3"><k>x</k>&cubic;</frm> estas
          egala al la volumeno de <tld/>o kun longo de &ccirc;iu latero
          <frm am="x"><k>x</k></frm>
          <fnt>
            <bib>Viki</bib>
            <lok><url ref="https://eo.wikipedia.org/?curid=243190">Kubo
            (algebro)</url></lok>
          </fnt>.
        </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>&c_k;&c_u;&c_b;</trd>,
       <trd>&c_t;&c_r;&c_e;&c_c;&c_ja;&c_ja; &c_s;&c_t;&c_u;&c_p;&c_je;&c_n;&c_mol;</trd>
     </trdgrp>
     <trd lng="en">cube</trd>
     <trd lng="es">cubo</trd>
     <trd lng="fa">&a_m;&f_k;&a_ayn;&a_b;</trd>
     <trd lng="hu">k&ouml;b</trd>
     <trd lng="id">kubik</trd>
     <trd lng="nl">derde <ind>macht</ind></trd>
     <trdgrp lng="pl">
       <trd>sze&sacute;cian<klr tip="ind"> (trzecia
pot&eogonek;ga)</klr></trd>,
       <trd>trzecia pot&eogonek;ga<klr tip="ind">
(liczby)</klr></trd></trdgrp>
     <trd lng="ro">treia putere</trd>
     <trdgrp lng="ru">
       <trd>&c_k;&c_u;&c_b;</trd>,
       <trd>&c_t;&c_r;&c_je;&c_t;&c_mol;&c_ja; &c_s;&c_t;&c_je;&c_p;&c_je;&c_n;&c_mol;</trd>
     </trdgrp>
     <trd lng="sv">kub</trd>
     <trd lng="zh">&#x7ACB;&#x65B9; <pr>l&#xEC;f&#x101;ng</pr></trd>
</snc>
   <ref tip="hom" cel="kub1.0o">Kubo (insulo)</ref>
   <trdgrp lng="cs">
      <trd>kostka</trd>,
      <trd>krychle</trd>,
      <trd>t&rcaron;et&iacute; mocnina</trd>
   </trdgrp>
   <trdgrp lng="de">
          <trd>W&uuml;rfel</trd>,
          <trd>Kubus<klr> [en ar&hcirc;itekturo]</klr></trd>
   </trdgrp>
   <trd lng="fr">cube</trd>
   <trdgrp lng="ja">
    <trd>&#x7ACB;&#x65B9;&#x4F53;  <pr>&#x308A;&#x3063;&#x307D;&#x3046;&#x305F;&#x3044;</pr></trd>,
    <trd>&#x7ACB;&#x65B9;  <pr>&#x308A;&#x3063;&#x307D;&#x3046;</pr></trd>,
    <trd>&#x30B5;&#x30A4;&#x30B3;&#x30ED;</trd>,
    <trd>&#x7ACB;&#x65B9;&#x5F62;&#x306E;&#x7269;  <pr>&#x305F;&#x307B;&#x3046;&#x3051;&#x3044;&#x306E;&#x3076;&#x3064;</pr></trd>
  </trdgrp>
  <trd lng="sk">tretia mocnina <klr>(&ccaron;&iacute;sla)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_u;&c_b;</trd>,
    <trd>&#x43A;&c_u;&c_b;&c_i;&#x43A;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x65B9;&#x5757; <pr>f&#x101;ngku&#xE0;i</pr></trd>,
    <trd>&#x65B9;&#x584A; <pr>f&#x101;ngku&#xE0;i</pr></trd>,
    <trd>&#x8272;&#x5B50; <pr>sh&#x1CE;izi</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kub.0a">
   <kap><tld/>a</kap>
   <fnt><bib>PIV1</bib></fnt>
   <snc mrk="kub.0a.rilata">
     <dif>
       Rilata al <tld/>o:
       <ekz>
         <tld/>a skatolo <klr>(<ref cel="kub.0forma"><tld/>forma</ref>)</klr>;
       </ekz>
      <ekz>
        oni konstruis akvujon, kiu povis enteni &ccirc;irka&ubreve; tri miliardojn da <tld/>aj metroj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
       <ekz>
         <uzo tip="fak">MAT</uzo>
         <tld/>a polinomo <klr>(triagrada)</klr>;
       </ekz>
       <ekz>
         <uzo tip="fak">MAT</uzo>
         <tld/>a <ref cel="radik.0o.per">radiko</ref>.
       </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_k;&c_u;&c_b;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>krychlov&yacute;</trd>,
    <trd>kubick&yacute;</trd>,
    <trd>t&rcaron;et&iacute;ho stupn&ecaron;</trd>
  </trdgrp>
  <trd lng="de">kubisch</trd>
  <trdgrp lng="en">
     <trd>cubic</trd>,
     <trd>cubical</trd>
   </trdgrp>
  <trd lng="es">c&uacute;bico</trd>
  <trdgrp lng="fa">
     <trd>&a_m;&f_k;&a_ayn;&a_b;&f_y;</trd>,
     <trd>&a_m;&f_k;&a_ayn;&a_b;</trd>
   </trdgrp>
  <trd lng="fr">cubique</trd>
  <trdgrp lng="hu">
     <trd>k&ouml;b-</trd>,
     <trd>kocka alak&uacute;</trd>
   </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7ACB;&#x65B9;&#x4F53;&#x306E;  <pr>&#x308A;&#x3063;&#x307D;&#x3046;&#x305F;&#x3044;&#x306E;</pr></trd>,
    <trd>&#x7ACB;&#x65B9;&#x5F62;&#x306E;  <pr>&#x305F;&#x307B;&#x3046;&#x3051;&#x3044;&#x306E;</pr></trd>,
    <trd>&#x7ACB;&#x65B9;&#x306E;  <pr>&#x305F;&#x304B;&#x305F;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">kubiek</trd>
  <trdgrp lng="pl">
     <trd>sze&sacute;cienny</trd>,
     <trd>kubiczny</trd>,
     <trd>trzeciego stopnia</trd>,
   </trdgrp>
  <trdgrp lng="ro">
    <trd>cub</trd>,
    <trd>cubic</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_u;&c_b;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">kub&aacute;nsky</trd>
  <trd lng="sv">kubisk</trd>
  <trd lng="uk">&#x43A;&c_u;&c_b;&c_i;&#x43D;&c_s;&#x44C;&#x43A;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x4E09;&#x6B21;&#x51AA; <pr>s&#x101;nc&#xEC;m&#xEC;</pr></trd>,
    <trd>&#x4E09;&#x6B21;&#x5E42; <pr>s&#x101;nc&#xEC;m&#xEC;</pr></trd>,
    <trd>&#x4E09;&#x6B21;&#x7684; <pr>s&#x101;nc&#xEC;de</pr></trd>
  </trdgrp>
</drv>

