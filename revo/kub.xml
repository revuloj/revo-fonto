<drv mrk="kub.0ismo">
   <kap><tld/>ismo</kap>
   <uzo tip="fak">BELA</uzo>
   <snc mrk="kub.0ismo.stilo">
     <dif>
       Skulpta kaj pentra <ref tip="super" cel="stil.0o.BELA.loka">stilo</ref> franc-devena,
       prospera komence de la 20a jarcento (proksimume 1910&ndash;1930) precipe en Francujo kaj
       &Ccirc;e&hcirc;ujo, influita de afrikaj arta&jcirc;oj, montranta simpligitajn,
       geometriajn formojn de objektoj, miksanta plurajn vid-punktojn al ili,
       algluanta al la verkoj materialojn el la bildigita&jcirc;oj (gazetfoliojn kaj
       aliajn):
      <ekz>
        inspirita de <tld/>ismo li e&ccirc; mane fabrikis 13 violonojn
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2014/011335.php">Pentristo de la homo kaj de la naturo</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        e&ubreve;ropaj kritikistoj emas paroli pri realismo a&ubreve; <tld/>ismo
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="&Monato;/2006/008835.php">500 &ccirc;efverkoj en Monako</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_k;&c_u;&c_b;&c_ib;&c_z;&c_m;</trd>
  <trd lng="cs">kubizmus</trd>
  <trd lng="de">Kubismus</trd>
  <trd lng="en">cubism</trd>
  <trd lng="fa">&f_k;&a_w;&a_b;&f_y;&a_s;&a_m;</trd>
  <trd lng="fr">cubisme</trd>
  <trd lng="hu">kubizmus</trd>
  <trd lng="id">kubisme</trd>
  <trdgrp lng="ja">
    <trd>&#x30AD;&#x30E5;&#x30D3;&#x30B9;&#x30E0;</trd>,
    <trd>&#x7ACB;&#x4F53;&#x6D3E;  <pr>&#x308A;&#x3063;&#x305F;&#x3044;&#x306F;</pr></trd>
  </trdgrp>
  <trd lng="nl">kubisme</trd>
  <trd lng="pl">kubizm</trd>
  <trd lng="ro">cubism</trd>
  <trd lng="ru">&c_k;&c_u;&c_b;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">kubizmus</trd>
  <trd lng="sv">kubism</trd>
  <trd lng="uk">&#x43A;&c_u;&c_b;&c_ib;&c_z;&#x43C;</trd>
  <trdgrp lng="zh">
    <trd>&#x7ACB;&#x4F53;&#x6D3E; <pr>L&#xEC;t&#x1D0;p&#xE0;i</pr></trd>,
    <trd>&#x7ACB;&#x9AD4;&#x6D3E; <pr>L&#xEC;t&#x1D0;p&#xE0;i</pr></trd>,
    <trd>&#x7ACB;&#x4F53;&#x4E3B;&#x4E49; <pr>l&#xEC;t&#x1D0;zh&#x1D4;y&#xEC;</pr></trd>,
    <trd>&#x7ACB;&#x9AD4;&#x4E3B;&#x7FA9; <pr>l&#xEC;t&#x1D0;zh&#x1D4;y&#xEC;</pr></trd>,
    <trd>&#x7ACB;&#x4F53;&#x6D3E; <pr>l&#xEC;t&#x1D0;p&#xE0;i</pr></trd>,
    <trd>&#x7ACB;&#x9AD4;&#x6D3E; <pr>l&#xEC;t&#x1D0;p&#xE0;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kub.jxet0o">
  <kap>&jcirc;et<tld/>o</kap>
  <snc mrk="kub.jxet0o.SPO">
  <uzo tip="fak">SPO</uzo>
    <dif>
      <tld lit="K"/>o uzata en iuj hazardludoj:
      <ekz>
        iu metis sur la tero grandan nigran &jcirc;et<tld/>on
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        duono de rompita &jcirc;et<tld/>o de la feli&ccirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>W&uuml;rfel</trd>,
      <trd>Spielw&uuml;rfel</trd>
    </trdgrp>
    <trd lng="en">die <klr>(1 die, 2 dice)</klr></trd>
    <trd lng="es">dado <klr>(de juego)</klr></trd>
    <trd lng="fr">d&eacute;</trd>
    <trd lng="hu">dob&oacute;kocka</trd>
    <trd lng="id">dadu</trd>
    <trd lng="it">dado<klr> (da gioco)</klr></trd>
    <trd lng="nl">dobbelsteen</trd>
    <trd lng="pl">kostka<klr> do gry</klr></trd>
    <trd lng="ru">&c_i;&c_g;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
    <trdgrp lng="zh">
    <trd>&#x9AB0;&#x5B50; <pr>t&#xF3;uzi</pr></trd>,
    <trd>&#x6474; <pr>sh&#x16B;</pr></trd>,
    <trd>&#x8272;&#x5B50; <pr>sh&#x1CE;izi</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_o;&c_s;&c_mol;&c_c;&c_mol; <klr tip="amb">(&c_g;&c_u;&c_l;&c_mol;&c_n;&c_je;&c_v;&c_a;&c_ja;)</klr></trd>,
    <trd>&c_k;&c_u;&c_b;&c_ib;&c_k; <klr>(&c_g;&c_u;&c_l;&c_mol;&c_n;&c_je;&c_v;&c_y;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30B5;&#x30A4;&#x30B3;&#x30ED;</trd>,
    <trd>&#x30C0;&#x30A4;&#x30B9;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_u;&c_b;&c_i;&#x43A; (&c_d;&#x43B;&#x44F; &#x43A;&c_i;&c_d;&c_a;&#x43D;&#x43D;&#x44F;)</trd>,
    <trd>&c_g;&c_r;&c_a;&#x43B;&#x44C;&#x43D;&c_a; &#x43A;&c_ib;&c_s;&c_t;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="kub.0forma">
  <kap><tld/>forma</kap>
  <snc>
    <dif>
      Havanta formon de <tld/>o:
      <ekz>
        li volas eskapi el tiu &ccirc;i stranga karcero <tld/>forma
        <fnt>
       <aut>Marteno Ecott</aut>
          <vrk><url ref="&Monato;/2000/006838.html">Inkubo en kubo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 17</lok>
        </fnt>.
      </ekz>
      <ekz>
        <tld/>forma satelito, malpli ol 500 gramojn peza
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2014/011232.php">Kosma kubo</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
