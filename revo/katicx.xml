<vortaro>
<art mrk="$Id: katicx.xml,v 1.4 2022/05/01 08:09:24 revo Exp $">
<kap>
    <rad>kati&ccirc;</rad>/o
</kap>
<drv mrk="katicx.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="katicx.0o.virkato">
    <dif>
      <klr>(Vd <ref cel="icx.0">-i&ccirc;</ref>)</klr>
      <ref tip="dif" cel="kat.vir0o">Virkato</ref>:
      <ekz>
        du junaj kaj amikaj <tld/>oj
        <fnt>
          <aut>Andy West</aut>
          <vrk>La Po&scirc;to Dismeo: Dua duono de 2000</vrk>
          <lok>
            <url ref="http://www.andywest.org/eo/numeroj/a00p2.html">2a
            junio 2000</url>
          </lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i estis tro granda por <tld/>o
        <fnt>
          <aut>Andy West</aut>
          <vrk>La Po&scirc;to Dismeo: Unua duono de 2002</vrk>
          <lok>
            <url ref="http://www.andywest.org/eo/numeroj/a02p1.html">11a
            aprilo 2002</url>
          </lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u <tld/>o ofte vizitas la idojn
        <fnt>
          <aut>Luiz Portella</aut>
          <vrk>&Scirc;i estas mia amiko</vrk>
          <lok>
            Torixor&eacute;u Libroj, jaro 2005a, &ccirc;apitro 9a, p. 31a
            <url
            ref="&Viki;/%C5%9Ci_estas_mia_amiko">En
            la domo de Marcelo</url>
          </lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
</drv>
</art>

