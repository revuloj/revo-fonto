  <trdgrp lng="de">
    <trd>ehemalig</trd>,
    <trd>fr&uuml;her</trd>
  </trdgrp>
  <trd lng="es">jubilado</trd>
  <trdgrp lng="fr">
    <trd>ancien<klr tip="ind"> (qui n'est plus)</klr></trd>,
    <trd>&agrave; la <ind>retraite</ind></trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5B0;&#x5E9;&#x5C1;&#x5B6;&#x5E2;&#x5B8;&#x5D1;&#x5B7;&#x5E8;</trd>
  <trdgrp lng="hu">
    <trd>valamikori</trd>,
    <trd>egykori</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x524D;&#x4EFB;&#x306E;  [<ind>&#x305C;&#x3093;&#x306B;&#x3093;&#x306E;</ind>]</trd>,
    <trd>&#x5143;&#x306E;  [<ind>&#x3082;&#x3068;&#x306E;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>by&lstroke;y</trd>,
    <trd>dawny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_y;&c_v;&c_sh;&c_i;&c_j;</trd>,
    <trd>&c_o;&c_t;&c_s;&c_t;&c_a;&c_v;&c_n;&c_o;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">b&yacute;val&yacute;</trd>
</drv>
  <drv mrk="eks.0igi">
    <kap><tld/>igi</kap>
    <snc mrk="eks.0igi.iun">
      <dif>
        <klr>(iun)</klr>
        Senigi je ofico, funkcio, posteno, rango, membreco:
        <!-- Senoficigi, forliberigi: -->
        <ekz>
          unu el la eminentaj oficiroj petis A&ubreve;guston <tld/>igi lin
          de la servo kaj lasi al li la pension
          <fnt><bib>FK</bib>, <lok>III, Anekdotoj</lok></fnt>;
        </ekz><ekz>
          post la milito multe da soldatoj estis <tld/>igitaj
          <fnt>
            <bib>ElektFab</bib>
            <lok>Kamarado Gaja</lok>
          </fnt>;
        </ekz><ekz>
          se vi tuj ne &ccirc;esigos la kunvenon, ni <tld/>igos vin el la
          sindikato!
          <fnt><bib>Metrop</bib>, <lok>&ccirc;apitro 5a</lok></fnt>;
        </ekz><ekz>
          <uzo tip="stl">FIG</uzo>
          kial mi estas <tld/>igita el la vivo?
          <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 19a</lok></fnt>;
        </ekz><ekz>
          <ind>ne<tld/>igebla</ind> funkciulo.
          <trdgrp lng="es">
             <trd>funcionario</trd>,
             <trd>no despedible</trd>
          </trdgrp>
          <trd lng="fr">inamovible<klr tip="ind">
           (qu'on ne peut renvoyer)</klr></trd>
          <trd lng="hu">elmozd&iacute;thatatlan<klr tip="ind">
          (hivatal&aacute;b&oacute;l)</klr></trd>
        </ekz>
      </dif>
      <refgrp tip="sub">
        <ref cel="dung.mal0i">maldungi</ref>,
        <ref cel="ofic.sen0igi">senoficigi</ref>
      </refgrp>
      <refgrp tip="vid">
        <ref cel="etat.0o.personaro">etato</ref>
      </refgrp>
      <trdgrp lng="ru">
        <trd>&c_u;&c_v;&c_o;&c_l;&c_i;&c_t;&c_mol; &c_v; &c_o;&c_t;&c_s;&c_t;&c_a;&c_v;&c_k;&c_u;</trd>,
        <trd>&c_i;&c_s;&c_k;&c_l;&c_ch;&c_i;&c_t;&c_mol;<klr> (&c_i;&c_z; &c_o;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_a;&c_c;&c_i;&c_i;)</klr></trd>
      </trdgrp>
    </snc>
    <snc mrk="eks.0igi.POL">
      <uzo tip="fak">POL</uzo>
      <dif>
        <klr>(kolektivan <ref cel="organ.0o.POL">organon<sncref/></ref>)</klr>
        <tld lit="E"/>igi<sncref ref="eks.0igi.iun"/> &ccirc;iujn membrojn
        de parlamento a&ubreve; alia kolektiva
        <ref cel="organ.0o.POL">organo<sncref/></ref> por ebligi
        elekton a&ubreve; nomumon de ties nova membraro:
        <ekz>
          Do, prefere ne estu ja nova prezidento, kiu povus <tld/>igi la
          parlamenton kaj ka&ubreve;zi novan &gcirc;eneralan vo&ccirc;donadon
          <fnt><aut>Carlo MINNAJA</aut>: <vrk><url ref="&Monato;/2022/013201.php">Farso en
          Italio</url></vrk>.
            <bib>Monato</bib> <lok>2022/04, p. 3</lok></fnt>.
        </ekz>
      </dif>
      <ref tip="vid" cel="fond.mal0i.POL">malfondi</ref>
      <trd lng="en">dissolve<klr> (parliament etc)</klr></trd>
      <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol;<klr> (&c_p;&c_a;&c_r;&c_l;&c_a;&c_m;&c_je;&c_n;&c_t; &c_i; &c_t;.&c_p;.)</klr></trd>
    </snc>
    <snc mrk="eks.0igi.ion">
      <uzo tip="stl">RAR</uzo>
      <dif>
        <klr>(ion)</klr>
        Igi <tld/>a, ne plu uzata:
        <ekz>
          nur tiam oni povus anonci, ke la nuna formo de la lingvo
          internacia estas <tld/>igata kaj anstata&ubreve; &gcirc;i eniras en la
          vivon la formo nova
          <fnt><bib>EE</bib>, <lok>&ccirc;apitro 7a</lok></fnt>.
        </ekz>
      </dif>
      <refgrp tip="sin">
        <ref cel="for.0igi">forigi</ref>,
        <ref cel="nul0.0igi.KOMUNE">nuligi<sncref/></ref>
      </refgrp>
      <trd lng="ru">&c_o;&c_t;&c_m;&c_je;&c_n;&c_i;&c_t;&c_mol;</trd>
    </snc>
    <trdgrp lng="be">
        <trd>&c_z;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_ib;&c_c;&c_mol;</trd>,
        <trd>&c_a;&c_d;&c_p;&c_r;&c_a;&c_v;&c_ib;&c_c;&c_mol; &c_u;
        <ind>&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_k;&c_u;</ind></trd>,
        <trd>&c_s;&c_k;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol; &c_u;
        <ind>&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_k;&c_u;</ind></trd>
      </trdgrp>
    <trdgrp lng="cs">
      <trd>odvolat</trd>,
      <trd>propustit <klr>(ze zam&ecaron;stn&aacute;n&iacute;, z funkce)</klr></trd>,
      <trd>sesadit</trd>,
      <trd>vylou&ccaron;iv</trd>,
      <trd>vylu&ccaron;ovat</trd>
    </trdgrp>
    <trdgrp lng="de">
        <trd>entlassen</trd>,
        <trd>beseitigen</trd>
      </trdgrp>
    <trdgrp lng="es">
      <trd>despedir <klr>(de un cargo o puesto laboral)</klr></trd>,
      <trd>expulsar <klr>(de un cargo o puesto laboral)</klr></trd>,
      <trd>deponer</trd>,
      <trd>destituir</trd>
    </trdgrp>
    <trdgrp lng="fr">
        <trd>cong&eacute;dier<klr tip="ind"> (licencier)</klr></trd>
        <trd>chasser<klr tip="ind"> (licencier)</klr></trd>
        <trd><ind>d&eacute;gager</ind> de ses obligations</trd>
        <trd><ind>d&eacute;mettre</ind> de ses fonctions</trd>
        <trd>d&eacute;missionner<klr tip="ind"> (tr, d&eacute;mettre)</klr></trd>
        <trd>d&eacute;mobiliser<klr tip="ind"> (un militaire)</klr></trd>
        <trd>d&eacute;poser<klr tip="ind"> (un monarque)</klr></trd>
        <trd>d&eacute;sengager<klr tip="ind"> (un militaire)</klr></trd>
        <trd>exclure<klr tip="ind"> (d'une organisation)</klr></trd>
        <trd>&eacute;vincer<klr tip="ind"> (d'une fonction)</klr></trd>
        <trd>licencier<klr tip="ind"> (limoger)</klr></trd>
        <trd>limoger</trd>
        <trd><ind>radier</ind> des effectifs</trd>
        <trd><ind>rayer</ind> de dessus l'&eacute;tat<klr tip="ind"> (vx,
        limoger)</klr></trd>
        <trd><ind>rayer</ind> des cadres</trd>
        <trd><ind>rayer</ind> des effectifs</trd>
        <trd><ind>relever</ind> de ses fonctions</trd>
        <trd>remercier<klr tip="ind"> (licencier)</klr></trd>
        <trd>renvoyer<klr tip="ind"> (licencier)</klr></trd>
        <trd>r&eacute;voquer<klr tip="ind"> (limoger)</klr></trd>
        <trd>mettre &agrave; la <ind>porte</ind><klr tip="ind">
        (licencier)</klr></trd>
        <trd>lourder<klr tip="ind"> (licencier)</klr></trd>
        <trd>vider<klr tip="ind"> (fam., licencier)</klr></trd>
        <trd>virer<klr tip="ind"> (fam., licencier)</klr></trd>
      </trdgrp>
    <trd lng="he">&#x5DC;&#x5E4;&#x5D8;&#x5E8;</trd>
  <trdgrp lng="hu">
        <trd>elbocs&aacute;t<klr tip="ind">
        (&aacute;ll&aacute;s&aacute;b&oacute;l)</klr></trd>,
        <trd>felment<klr tip="ind">
        (hivatal&aacute;b&oacute;l)</klr></trd>
      </trdgrp>
    <trdgrp lng="ja">
    <trd>&#x514D;&#x8077;&#x3059;&#x308B;  [<ind>&#x3081;&#x3093;&#x3057;&#x3087;&#x304F;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x89E3;&#x4EFB;&#x3059;&#x308B;  [<ind>&#x304B;&#x3044;&#x306B;&#x3093;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="pl">
        <trd>usun&aogonek;&cacute;</trd>,
        <trd>wydali&cacute;</trd>,
        <trd>zwolni&cacute;</trd>,
      </trdgrp>
    <trd lng="sk">vyl&uacute;&ccaron;i&tcaron; zo spolo&ccaron;enstva</trd>
  </drv>

  <drv mrk="eks.0igo">
    <kap><tld/>igo</kap>
    <snc mrk="eks.0igo.KOMUNE">
      <dif>
        Senigo je anta&ubreve;a funkcio: 
        <ekz>
          mi estas kontra&ubreve; la <ind><tld/>igo</ind> de Ivagin el la
          partio
          <fnt><bib>Metrop</bib>, <lok>&ccirc;apitro 7a</lok></fnt>;
        </ekz><ekz>
          mi ricevis <tld/>igon kaj krom &gcirc;i nur panbulon kaj kvar
          gro&scirc;ojn
          <fnt><bib>ElektFab</bib> <lok>Kamarado Gaja</lok></fnt>.
        </ekz>
      </dif>
      <trdgrp lng="be">
        <trd>&c_z;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
        <trd>&c_p;&c_a;&c_z;&c_b;&c_a;&c_w;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je; <ind>&c_p;&c_a;&c_s;&c_a;&c_d;&c_y;</ind></trd>
      </trdgrp>
      <trdgrp lng="de">
         <trd>Entlassung</trd>,
         <trd>K&uuml;ndigung</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_u;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>,
        <trd>&c_o;&c_t;&c_s;&c_t;&c_a;&c_v;&c_k;&c_a;</trd>,
        <trd>&c_s;&c_n;&c_ja;&c_t;&c_i;&c_je; &c_s; &c_d;&c_o;&c_l;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_i;, &c_s; &c_p;&c_o;&c_s;&c_t;&c_a;</trd>,
        <trd>&c_o;&c_s;&c_v;&c_o;&c_b;&c_o;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je; &c_o;&c_t; &c_d;&c_o;&c_l;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_i;</trd>
      </trdgrp>
    </snc>
    <snc mrk="eks.0igo.POL">
      <uzo tip="fak">POL</uzo>
      <dif>
        La ago <ref cel="eks.0igi.POL"><tld/>igi<sncref/></ref>
        kolektivan <ref cel="organ.0o.POL">organon<sncref/></ref>:
        <ekz>
          la oficiala letero de la Varsovia &Ccirc;efurba Oficejo, per kiu
          almena&ubreve; morale nuligi&gcirc;as la decido de 17 okt 1985 pri
          <tld/>igo de la tiama &Ccirc;efa Estraro de PEA
          <fnt><aut>HeKo</aut>:
            <vrk>Gazetoj: Literatura Foiro. 2003. &#x2116;201</vrk>
            <bib>LOdE</bib></fnt>.
        </ekz>
      </dif>
      <trd lng="en">dissolution<klr> (of parliament etc)</klr></trd>
      <trd lng="ru">&c_r;&c_o;&c_s;&c_p;&c_u;&c_s;&c_k;<klr> (&c_p;&c_a;&c_r;&c_l;&c_a;&c_m;&c_je;&c_n;&c_t;&c_a; &c_i; &c_t;.&c_p;.)</klr></trd>
    </snc>
    <trdgrp lng="cs">
      <trd>absolutorium</trd>,
      <trd>odvol&aacute;n&iacute;</trd>,
      <trd>propu&scaron;t&ecaron;n&iacute;</trd>,
      <trd>sesazen&iacute;</trd>,
      <trd>svr&zcaron;en&iacute;</trd>,
      <trd>zbaven&iacute; funkce</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>despido</trd>,
      <trd>expulsi&oacute;n</trd>,
      <trd>destituci&oacute;n</trd>
    </trdgrp>
    <trdgrp lng="fr">
        <trd>cong&eacute;diement</trd>
        <trd>d&eacute;position<klr tip="ind"> (d'un monarque)</klr></trd>,
        <trd>exclusion<klr tip="ind"> (d'une organisation)</klr></trd>
        <trd>licenciement</trd>
        <trd>limogeage</trd>
        <trd>renvoi<klr tip="ind"> (licenciement)</klr></trd>
