<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>eks</rad>-
</kap>
<subart>
  <dif>
    Prefikso signifanta, ke oni forlasis profesion, okupon, staton; iama,
    estinta kaj ne plu estanta:
  </dif>
  <snc>
    <ekz>
      <tld/>kapitano, <tld/>oficisto, <tld/>fabrikanto, <tld/>re&gcirc;o,
      <tld/>kutima, <tld/>edzi&gcirc;i.
    </ekz>
  </snc>
  <trdgrp lng="de">
    <trd>Ex-</trd>,
    <trd>a.D.</trd>
  </trdgrp>
  <trd lng="es">ex-</trd>
  <trd lng="fr">ex-</trd>
  <trdgrp lng="hu">
    <trd>volt</trd>,
    <trd>ex-</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_k;&c_s;-</trd>,
    <trd>&c_b;&c_y;&c_v;&c_sh;&c_i;&c_j;</trd>,
    <trd>&c_v; &c_o;&c_t;&c_s;&c_t;&c_a;&c_v;&c_k;&c_je;</trd>
  </trdgrp>
</subart>

<subart>
  <dif>
    Samsignifa memstara vortero:
  </dif>
  <drv mrk="eks.0a">
    <kap><tld/>a</kap>
    <snc>
      <dif>
        Estinta:
        <ekz>
          tiu urbo estas <tld/>a fortika&jcirc;o de la Romianoj.
        </ekz>
      </dif>
    </snc>
    <trd lng="cs">b&yacute;val&yacute;</trd>
    <trdgrp lng="de">
      <trd>ehemalig</trd>,
      <trd>fr&uuml;her</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>ancien<klr tip="ind"> (qui n'est plus)</klr></trd>,
      <trd>&agrave; la <ind>retraite</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>valamikori</trd>,
      <trd>egykori</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_b;&c_y;&c_v;&c_sh;&c_i;&c_j;</trd>,
      <trd>&c_o;&c_t;&c_s;&c_t;&c_a;&c_v;&c_n;&c_o;&c_j;</trd>
    </trdgrp>
  </drv>
  <drv mrk="eks.0igi">
    <kap><tld/>igi</kap>
    <snc>
      <dif>
        Senoficigi, forliberigi:
        <ekz>
          <ind>ne<tld/>igebla</ind> funkciulo.
          <trd lng="fr">inamovible<klr tip="ind"> (qu'on ne peut
          renvoyer)</klr></trd>
          <trd lng="hu">elmozd&iacute;thatatlan<klr tip="ind">
          (hivatal&aacute;b&oacute;l)</klr></trd>
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="de">
      <trd>entlassen</trd>,
      <trd>beseitigen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;mettre<klr tip="ind"> (qn de ses fonctions)</klr></trd>,
      <trd>exclure<klr tip="ind"> (renvoyer)</klr></trd>,
      <trd>renvoyer<klr tip="ind"> (r&eacute;voquer)</klr></trd>,
      <trd>r&eacute;voquer<klr tip="ind"> (relever de ses
      fonctions)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>elbocs&aacute;t<klr tip="ind">
      (&aacute;ll&aacute;s&aacute;b&oacute;l)</klr></trd>,
      <trd>felment<klr tip="ind">
      (hivatal&aacute;b&oacute;l)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_u;&c_v;&c_o;&c_l;&c_i;&c_t;&c_mol; &c_v; &c_o;&c_t;&c_s;&c_t;&c_a;&c_v;&c_k;&c_u;</trd>
  </drv>
  <drv mrk="eks.0igxi">
    <kap><tld/>i&gcirc;i</kap>
    <snc>
      <dif>
        Memvole a&ubreve; kontra&ubreve;vole forlasi sian oficon,
        postenon, rangon:
        <ekz>
          ili malebligis pluan karieron de anoncisto; do, li <tld/>i&gcirc;is
          <fnt><bib>MkM</bib>, <lok>epilogo, p. 337a</lok></fnt>.
        </ekz>
      </dif>
      <ref tip="super" cel="rezign.0i">rezigni</ref>
      <refgrp tip="sub">
        <ref cel="abdik.0i">abdiki</ref>,
        <ref cel="demisi.0i">demisii</ref>
      </refgrp>
    </snc>
    <rim>
      Komparon de la sinonimoj vd sub
      <ref cel="abdik.0o.RIM">abdiki</ref>.
    </rim>
    <trdgrp lng="de">
      <trd>weggehen</trd>,
      <trd>abdanken</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;missionner</trd>,
      <trd>se <ind>d&eacute;mettre</ind><klr tip="ind">
      (d&eacute;missionner)</klr></trd>,
      <trd>se <ind>retirer</ind><klr> (d'une fonction)</klr></trd>,
    </trdgrp>
    <trdgrp lng="hu">
      <trd>lek&ouml;sz&ouml;n</trd>,
      <trd>lemond</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_y;&c_j;&c_t;&c_i; &c_v; &c_o;&c_t;&c_s;&c_t;&c_a;&c_v;&c_k;&c_u;</trd>
  </drv>
  <drv mrk="eks.0igxo">
    <kap><tld/>i&gcirc;o</kap>
    <snc>
      <dif>
        Rezigno je propra funkcio, okupo, stato.
      </dif>
    </snc>
    <trdgrp lng="fr">
      <trd>d&eacute;mission</trd>,
      <trd>d&eacute;part<klr> (d&eacute;mission)</klr></trd>,
      <trd>retrait<klr> (d&eacute;mission)</klr></trd>
    </trdgrp>
  </drv>
</subart>
</art>
