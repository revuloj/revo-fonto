<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
 <ofc>*</ofc>
 <rad>bapt</rad>/i
</kap>
<drv mrk="bapt.0i">
 <kap><ofc>*</ofc><tld/>i</kap>
 <gra><vspec>tr</vspec></gra>
 <snc num="1">
  <dif>
   Rite kristanigi iun per akvo k doni al li apartan nomon. 
  </dif>
 </snc>
 <snc num="2">
  <uzo tip="klr">(f)</uzo>
  <dif>
   Doni nomon al iu a&ubreve; io: 
   <ekz><tld/>i &scirc;ipon, sonorilegon.</ekz>
  </dif>
 </snc>
 <trd lng="de">taufen</trd>
 <trd lng="en">baptize</trd>
 <trd lng="nl">dopen</trd>
</drv>
<drv mrk="bapt.0o">
 <kap><tld/>o</kap>
 <snc>
  <dif>
   Religia a&ubreve; civila ceremonio per  kiu oni <tld/>as. 
  </dif>
 </snc>
 <trd lng="de">Taufe</trd>
</drv>
<drv mrk="bapt.0ato">
 <kap><tld/>ato</kap>
 <snc>
  <dif>
   Tiu, kiu ricevas la <tld/>on. 
  </dif>
 </snc>
</drv>
<drv mrk="bapt.0opatro">
 <kap><tld/>opatro</kap>
 <snc>
  <dif>
   Tiu, kiu prezentas infanon por <tld/>o,
   donante al &gcirc;i &gcirc;ian anta&ubreve;nomon. 
  </dif>
 </snc>
 <trd lng="de">Taufpate</trd>
</drv>
<drv mrk="bapt.0ofilo">
 <kap><tld/>ofilo</kap>
 <snc>
  <dif>
   La <tld/>ito rilate al la <tld/>opatro. 
  </dif>
 </snc>
 <trd lng="de">Patenkind</trd>
</drv>
<drv mrk="bapt.0nomo">
 <kap><tld/>nomo</kap>
 <snc>
  <dif>
   Anta&ubreve;nomo.  
  </dif>
 </snc>
</drv>
</art>
