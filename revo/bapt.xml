<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>bapt</rad>/i
    </kap>

    <drv mrk="bapt.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="bapt.0i.kri">
        <uzo tip="fak">KRI</uzo>
        <dif>
          Rite kristanigi iun per akvo kaj doni al li apartan nomon.
        </dif>
        <ref tip="super" cel="sakram.0o.kri">sakramento</ref>
        <trd lng="fr">baptiser</trd>
        <trd lng="hu">keresztel</trd>
        <trdgrp lng="ru">
          <trd>&c_k;&c_r;&c_je;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>,
          <trd>&c_k;&c_r;&c_je;&c_shch;&c_a;&c_t;&c_mol;</trd>,
          <trd>&c_o;&c_k;&c_r;&c_je;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
        </trdgrp>
      </snc>
      <snc>
        <uzo tip="stl">FIG</uzo>
        <dif>
          Doni nomon al iu a&ubreve; io:
          <ekz>
            <tld/>i &scirc;ipon, sonorilegon.</ekz>
        </dif>
        <ref tip="super" cel="inauxg.0i">ina&ubreve;guri</ref>
        <trdgrp lng="fr">
          <trd>baptiser <klr tip="ind">(d&eacute;nommer)</klr></trd>,
          <trd>d&eacute;nommer <klr tip="ind">(donner un nom)</klr></trd></trdgrp>
        <trdgrp lng="hu">
          <trd>felszentel <klr tip="ind">(nevet ad)</klr></trd>,
          <trd>nevet ad</trd></trdgrp>
        <trdgrp lng="ru">
          <trd>&c_d;&c_a;&c_t;&c_mol; &c_i;&c_m;&c_ja;</trd>,
          <trd>&c_o;&c_s;&c_v;&c_ja;&c_t;&c_i;&c_t;&c_mol;</trd>
        </trdgrp>
      </snc>
      <trd lng="de">taufen</trd>
      <trd lng="en">baptize</trd>
      <trd lng="es">bautizar</trd>
      <trd lng="nl">dopen</trd>
      <trd lng="sk">krsti&tcaron;</trd>
      <trd lng="sv">d&ouml;pa</trd>
    </drv>

    <drv mrk="bapt.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Religia a&ubreve; civila ceremonio per kiu oni <tld/>as.
        </dif>
      </snc>
      <trd lng="de">Taufe</trd>
      <trd lng="en">baptism</trd>
      <trd lng="es">bautizo</trd>
      <trd lng="fr">bapt&ecirc;me</trd>
      <trdgrp lng="hu">
        <trd>keresztel&odblac;</trd>,
        <trd>keresztel&eacute;s</trd></trdgrp>
      <trd lng="nl">doopsel</trd>
      <trd lng="ru">&c_k;&c_r;&c_je;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>
      <trd lng="sk">krst</trd>
      <trd lng="sv">dop</trd>
    </drv>

    <drv mrk="bapt.0ato">
      <kap><tld/>ato</kap>
      <snc>
        <dif>
          Tiu, kiu ricevas la <tld/>on.
        </dif>
      </snc>
      <trd lng="fr">baptis&eacute;</trd>
      <trd lng="hu">keresztelend&odblac;</trd>
      <trd lng="nl">dopeling</trd>
      <trd lng="ru">&c_k;&c_r;&c_je;&c_shch;&c_a;&c_je;&c_m;&c_y;&c_j;</trd>
    </drv>

    <drv mrk="bapt.0ano">
      <kap><tld/>ano</kap>
      <snc>
        <dif>
          <ref cel="bapt.0opatro"><tld lit="B"/>opatro</ref>
	  rilate al la <tld/>opatrino a&ubreve; al la gepatroj de <tld/>ato:
	  <ekz>
	    jen kion li skribas:
	    &leftquot;kara amiko, <tld/>ano kaj bonfaranto!&rightquot;
	    <fnt><bib>Revizoro</bib>, <lok>1:1</lok></fnt>.
	  </ekz>
        </dif>
      </snc>
      <trd lng="ru">&c_k;&c_u;&c_m;</trd>
    </drv>

    <drv mrk="bapt.0anino">
      <kap><tld/>anino</kap>
      <snc>
        <dif>
          <ref cel="bapt.0opatrino"><tld lit="B"/>opatrino</ref>
	  rilate al la <tld/>opatro a&ubreve; al la gepatroj de <tld/>ato:
	  <ekz>
	    denunci lin pro tio, ke li geedzigis vendiston Mikelon kun
	    lia <tld/>anino
	    <klr>(&ccirc;e la ortodoksaj kristanoj geedzi&gcirc;o inter
	    ge<tld/>anoj estas malpermesita)</klr>
	    <fnt><aut>N. Gogol, tr. V. Vy&ccirc;eg&jcirc;anin</aut>:
	      <vrk>Malvivaj animoj</vrk>, <lok>&ccirc;ap. 10</lok>.
	    </fnt>.
	  </ekz>
        </dif>
      </snc>
      <trd lng="ru">&c_k;&c_u;&c_m;&c_a;</trd>
    </drv>

    <drv mrk="bapt.0opatro">
      <kap><ofc>*</ofc><tld/>opatro</kap>
      <snc>
        <dif>
          Tiu viro, kiu prezentas infanon por <tld/>o,
          donante al &gcirc;i &gcirc;ian anta&ubreve;nomon.
        </dif>
      </snc>
      <trd lng="de">Taufpate</trd>
      <trd lng="en">godfather</trd>
      <trd lng="es">padrino</trd>
      <trdgrp lng="fr">
        <trd>parrain</trd></trdgrp>
      <trd lng="hu">keresztapa</trd>
      <trdgrp lng="nl">
        <trd>peter</trd>,
        <trd>peetvader</trd>
      </trdgrp>
      <trd lng="ru">&c_k;&c_r;&c_jo;&c_s;&c_t;&c_n;&c_y;&c_j; &c_o;&c_t;&c_je;&c_c;</trd>
      <trd lng="sk">krstn&yacute; otec</trd>
    </drv>

    <drv mrk="bapt.0opatrino">
      <kap><tld/>opatrino</kap>
      <snc>
        <dif>
          Tiu virino, kiu prezentas infanon por <tld/>o,
          donante al &gcirc;i &gcirc;ian anta&ubreve;nomon.
        </dif>
      </snc>
      <trd lng="es">madrina</trd>
      <trd lng="en">godmother</trd>
      <trdgrp lng="fr">
        <trd>marraine</trd></trdgrp>
      <trd lng="hu">keresztanya</trd>
      <trdgrp lng="nl">
        <trd>meter</trd>,
        <trd>peetmoeder</trd>
      </trdgrp>
      <trd lng="sk">krstn&aacute; matka</trd>
    </drv>

    <drv mrk="bapt.0ofilo">
      <kap><ofc>*</ofc><tld/>ofilo</kap>
      <snc>
        <dif>
          La <tld/>ito rilate al la <tld/>opatr(in)o.
        </dif>
      </snc>
      <trd lng="de">Patenkind</trd>
      <trd lng="en">godson</trd>
      <trd lng="es">ahijado</trd>
      <trd lng="fr">filleul(e)</trd>
      <trd lng="hu">keresztfi&udblac;</trd>
      <trd lng="nl">petekind</trd>
      <trd lng="ru">&c_k;&c_r;&c_je;&c_s;&c_t;&c_n;&c_i;&c_k;</trd>
    </drv>

    <drv mrk="bapt.0nomo">
      <kap><tld/>onomo</kap>
      <snc mrk="bapt.0nomo.propre">
        <uzo tip="fak">HIS</uzo>
        <uzo tip="fak">KRI</uzo>
        <dif>
          Nomo donita &ccirc;e <tld/>o.
        </dif>
	<trd lng="en">baptismal <ind>name</ind></trd>
	<trd lng="hu">keresztn&eacute;v</trd>
	<trd lng="nl">doopnaam</trd>
	<trd lng="ru">&c_k;&c_r;&c_jo;&c_s;&c_t;&c_n;&c_o;&c_je; &c_i;&c_m;&c_ja;</trd>
      </snc>
      <snc mrk="bapt.0nomo.antauxnomo">
        <dif>
          Anta&ubreve;nomo.
        </dif>
	<trd lng="en">first <ind>name</ind></trd>
	<trd lng="hu">ut&oacute;n&eacute;v</trd>
	<trd lng="nl">voornaam<klr tip="ind"> (doopnaam)</klr></trd>
      </snc>
      <rim>
        E&ccirc; inter la kristanoj ne &ccirc;iam kaj ne &ccirc;ie (la) <tld/>onomo
        koincidas kun anta&ubreve;nomo.  Ekz-e la Kieva &ccirc;efprinco Vladimiro
        Monoma&hcirc;o havis la laikan <ctl>princan anta&ubreve;nomon</ctl>
        Vladimiro kaj la <tld/>onomon Bazilo.  Kutime oni &scirc;an&gcirc;as la
        anta&ubreve;nomon &ccirc;e mona&hcirc;i&gcirc;o a&ubreve; papi&gcirc;o ktp.
      </rim>
      <trdgrp lng="fr">
        <trd>pr&eacute;nom</trd>,
        <trd>nom de bapt&ecirc;me</trd></trdgrp>
      <trd lng="ru">&c_l;&c_i;&c_ch;&c_n;&c_o;&c_je; <ind>&c_i;&c_m;&c_ja;</ind></trd>
      <trd lng="sk">krstn&eacute; meno</trd>
      <trd lng="sv">f&ouml;rnamn</trd>
    </drv>
  </art>
