<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>bord</rad>/o
</kap>
<drv mrk="bord.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Strio da tero tu&scirc;anta etenda&jcirc;on da akvo:
      <ekz>
        <tld/>o de rivero<fnt>Z</fnt>, de lago, de maro;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la registaro trovi&gcirc;is &ccirc;e la <tld/>o de l' senfunda&jcirc;o
        <fnt>(<aut>Kabe</aut>: <vrk>La Faraono, 3a vol.</vrk>,
        <lok>&ccirc;ap. 16</lok>)</fnt>
        <klr>(esti preska&ubreve; ruinigita)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sur la <tld/>o <klr>(tute proksime de,
        <ref tip="sin" cel="sojl.0o">sojle</ref> de)</klr>
        de la morto vi donas savon al la fremdulo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        malri&ccirc;a en sen<tld/>an <klr>(senliman,
        senhelpan)</klr> solecon &jcirc;etita virino<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rand.0o">rando</ref>,
      <ref cel="plauxd.0ejo">pla&ubreve;dejo</ref>,
      <ref cel="kaj1.0o">kajo</ref>,
      <ref cel="strand.0o">strando</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Ufer</trd>, <trd>Rand</trd>
  </trdgrp>
  <trd lng="en">shore</trd>
  <trd lng="es">costa</trd>
  <trdgrp lng="fr">
    <trd>c&ocirc;te<klr tip="ind"> (rivage)</klr></trd>,
    <trd>rive</trd>,
    <trd>rivage</trd>,
    <trd>bord<klr tip="amb"> (fig.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
     <trd>costa</trd>,
     <trd>margem</trd>,
     <trd>borda</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_r;&c_je;&c_g;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_j; <klr tip="amb">(f)</klr></trd>
  </trdgrp>
  <trd lng="sk">breh</trd>
  <adm>
  La nuntempa difino de tiu vorto ne kongruas kun &ccirc;iuj ekzemploj 
  donitaj, nek kun la derivajxo bord.super0igxi. Estas inde 
  pliprecizigi &gcirc;iajn subsencojn.<aut>HJ</aut>
  </adm>
</drv>

<drv mrk="bord.al0igxi">
  <kap>al<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Atingi la <tld/>on na&gcirc;ante a&ubreve; &scirc;ipante.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>anlegen</trd>, <trd>landen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>accoster <klr tip="ind">(intr.)</klr></trd>,
    <trd>aborder <klr tip="ind">(intr.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
     <trd>acostar</trd>,
     <trd>chegar <klr>(&agrave; margem, costa)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_ch;&c_a;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_s;&c_t;&c_a;&c_t;&c_mol; <klr>(&c_k; &c_b;&c_je;&c_r;&c_je;&c_g;&c_u;)</klr></trd>
  </trdgrp>
  <trd lng="sk">prist&aacute;&tcaron;</trd>
</drv>

<drv mrk="bord.al0igxejo">
  <kap>al<tld/>i&gcirc;ejo</kap>
  <snc>
    <dif>
      Loko aran&gcirc;ita por la al<tld/>i&gcirc;o de &scirc;ipoj.
    </dif>
  </snc>
  <ref tip="vid" cel="sxip.el0ejo">el&scirc;ipejo.</ref>
  <trd lng="fr">quai</trd>
  <trdgrp lng="pt">
     <trd>cais</trd>,
     <trd>p&iacute;er</trd>,
     <trd>atracadouro</trd>
  </trdgrp>  
<trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_s;&c_t;&c_a;&c_n;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_ch;&c_a;&c_l;</trd>
  </trdgrp>
  <trd lng="sk">pr&iacute;stav</trd>
</drv>

<drv mrk="bord.super0igxi">
  <kap>super<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Elver&scirc;i&gcirc;i super la <tld/>o, superakvi la <tld/>on
    </dif>
    <refgrp tip="vid">
      <ref cel="inund.0i">inundi</ref>.
    </refgrp>
  </snc>
   <trd lng="fr">d&eacute;border <klr tip="ind">(en p. d'une rivi&egrave;re)</klr></trd>
   <trd lng="pt">transbordar</trd>
   <trd lng="ru">&c_v;&c_y;&c_j;&c_t;&c_i; &c_i;&c_z; <ind>&c_b;&c_je;&c_r;&c_je;&c_g;&c_o;&c_v;</ind></trd>
</drv>
</art>
