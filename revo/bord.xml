<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bord.xml,v 1.34 2021/05/08 10:52:31 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bord</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="bord.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bord.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="super" cel="stri.0o.streko">Strio</ref> da tero tu&scirc;anta etenda&jcirc;on da <ref cel="akv.0o">akvo</ref>:
      <ekz>
        sur la <tld/>o de la maro staris amaso da homoj
        <fnt><bib>F</bib>, <lok>&FE; 32</lok></fnt>;
      </ekz>
      <ekz>
        jen supre sur la <tld/>o de la haveno en la lasta
        minuto rapide alkuras ankora&ubreve; kelkaj kale&scirc;oj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        sablo sur la <tld/>o de la maro
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 22:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen mi staras sur la <tld/>o de la rivero
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 41:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ordonis transiri al la alia <tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 8:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> transiris Jordanon <klr>[&#8230;]</klr>, kiam &gcirc;i inundis siajn amba&ubreve;
        <tld/>ojn
        <fnt>
          <bib>MT</bib>
          <lok>I. Kroniko 12:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Jordan</nom> estis plena &ccirc;e &ccirc;iuj siaj <tld/>oj dum la tuta tempo de rikoltado
        <fnt>
          <bib>MT</bib>
          <lok>Josuo 3:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        en vintro tie ne pasas &scirc;ipoj, tiam la tuta Sundo &gcirc;is la Sveda <tld/>o estas
        kovrita de glacio
        <fnt>
          <bib>Fab2</bib>
          <lok>Dano Holger</lok>
        </fnt>;
      </ekz>
      <ekz>
        malsupre &ccirc;e la <tld/>o ruli&gcirc;adis rosmaroj
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj mizeraj lignaj budoj <klr>[&#8230;]</klr> staris sur la transa <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Rejno &scirc;a&ubreve;me saltas &gcirc;is super sia <tld/>o
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="http://steloj.de/esperanto/butiko">La Stranga Butiko</url></vrk>
          <lok>Sibilla Orakolas</lok>
        </fnt>;
      </ekz>
      <ekz>
        akvo silenta subfosas la <tld/>on
        <klr>(ne subtaksu kio &scirc;ajnas kvieta)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;e la <tld/>o de la scenejo <klr>[staris]</klr> tri sinjorinoj
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="eskarp.0o">eskarpo</ref>,
      <ref cel="kaj1.0o.MAR">kajo<sncref/></ref>,
      <ref cel="klif.0o">klifo</ref>,
      <ref cel="plagx.0o">pla&gcirc;o</ref>,
      <ref cel="strand.0o">strando</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="dig.0o">digo</ref>,
      <ref cel="plauxd.0ejo">pla&ubreve;dejo</ref>,
      <ref cel="rand.0o">rando</ref>
    </refgrp>
  </snc>
  <snc mrk="bord.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Proksimo de <ref cel="ir.trans0i">transir</ref>o al alia stato:
      <ekz>
        la registaro trovi&gcirc;is &ccirc;e la <tld/>o de l'
        senfunda&jcirc;o
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 16</lok></fnt>
        <klr>(esti preska&ubreve; ruinigita)</klr>;
      </ekz>
      <ekz>
        la kolero el&jcirc;etas la homon sur la <tld/>ojn de la maljusteco
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        Vi estas nova fonto de feli&ccirc;o / Kaj donas sur la <tld/>o de 
        <klr>(tute proksime de, <ref tip="sin" cel="sojl.0o">sojle</ref> de)</klr>
        la morto / Al la fremdulo savon kaj revenon
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        malri&ccirc;a en sen<tld/>an <klr>(senliman,
        senhelpan)</klr> solecon &jcirc;etita virino
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="abism.0o">abismo</ref>,
       <ref cel="lim.0o.FIG">limo<sncref/></ref>,
       <ref cel="rand.0o.KOMUNE">rando<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_r;&c_a;&c_g;</trd>
  <trd lng="bg">&c_b;&c_r;&c_ja;&c_g;</trd>
  <trdgrp lng="cs">
    <trd>b&rcaron;eh</trd>,
    <trd>pob&rcaron;e&zcaron;&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ufer</trd>, <trd>Rand</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>shore</trd>,
    <trd>waterfront</trd>
  </trdgrp>
  <trd lng="es">orilla</trd>
  <trdgrp lng="fr">
    <trd>bord<klr tip="amb"> (fig.)</klr></trd>,
    <trd>rivage</trd>,
    <trd>rive</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&het;&vav;&fpe;</trd>,
    <trd>&gimel;&dalet;&he;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>oever</trd>,
    <trd>waterkant</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>brzeg</trd>,
    <trd>wybrze&zdot;e</trd>,
    <trd>nadbrze&zdot;e</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>costa</trd>,
    <trd>margem</trd>,
    <trd>borda</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_r;&c_je;&c_g;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_j; <klr tip="amb">(f)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>breh</trd>,
    <trd>pobre&zcaron;ie</trd>
  </trdgrp>
</drv>

<drv mrk="bord.0ede">
  <kap><tld/>e de</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="bord.0ede.KOMUNE">
    <dif>
      La&ubreve; la <tld/>o de, sur la <tld/>o de...:
      <ekz>
        sinjoro <nom>Claude Monet</nom> konduku nin al <nom>Giverny</nom> <tld/>e de Sejno
        <fnt>
          <aut>M. Proust, trad. P. Combot</aut>,
          <vrk>Pri la legado</vrk>,
          <lok>1993</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi facile trovos en normala atlaso, kie situas Bervalo <klr>[&#8230;]</klr> jes, tie,
        <tld/>e de la blua Mediteraneo
        <fnt>
          <bib>KrB</bib>
        </fnt>;
      </ekz>
      <ekz>
        nokte en diversaj lokoj <tld/>e de la lago brulis altaj fajroj
        <fnt>
          <bib>Monato</bib>
          <aut>Claude Piron</aut>
          <vrk><url ref="https://www.monato.be/2003/008141.html">Francio invitas, Svislando suferas</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili estas sur<tld/>e de la maro &ccirc;e Helesponto
        <fnt>
          <bib>Rsp</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e<tld/>e de Sudameriko
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        vico da restoracioj la&ubreve;<tld/>e de l&#8217; rivero
        <fnt>
          <bib>ChR</bib>
        </fnt>;
      </ekz>
      <ekz>
        amba&ubreve;<tld/>e de la rivero
        <fnt>
          <bib>Monato</bib>
          <aut>Jean-Louis Madzella/pg</aut>
          <vrk><url ref="https://www.monato.be/2006/009016.php">Plezuro elsakigita</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        najbaro <klr>[&#8230;]</klr> ali<tld/>e de la rivero
        <fnt>
          <bib>Monato</bib>
          <aut>AMik</aut>
          <vrk><url ref="https://www.monato.be/2011/010441.php">Rio-granda stimulado</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">am <ind>Ufer</ind> von</trd>
  <trd lng="fr">sur la rive de</trd>
</drv>

<drv mrk="bord.al0igxi">
  <kap>al<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="bord.al0igxi.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Atingi <tld/>on, na&gcirc;ante a&ubreve; &scirc;ipante:
      <ekz>
        tuj la &scirc;ipo al<tld/>i&gcirc;is tien, kien ili veturis
        <fnt>
          <bib>NT</bib>
          <lok>Johano 6:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        al<tld/>i&gcirc;inte &ccirc;e Sirakuso, ni restis tie tri tagojn
        <fnt><bib>NT</bib>, <lok>&Ago; 28:12</lok></fnt>;
      </ekz>
      <ekz>
        la lumigita &scirc;ipeto de la kronprinco al<tld/>i&gcirc;is inter krioj kaj kantoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fi&scirc;kaptistojn &ccirc;e ilia al<tld/>i&gcirc;o ili akceptas kun varma biero
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_ch;&c_a;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_s;&c_t;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_d;&c_a; &c_b;&c_je;&c_r;&c_a;&c_g;&c_a;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>anlegen</trd>, <trd>landen</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>atracar</trd>,
    <trd>abordar <klr>(llegar a puerto)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>accoster<klr tip="ind"> (intr.)</klr></trd>,
    <trd>aborder<klr tip="ind"> (intr.)</klr></trd>
  </trdgrp>
  <trd lng="he">&lamed;&ayin;&gimel;&vav;&fnun;</trd>
  <trdgrp lng="nl">
    <trd>aan land gaan</trd>,
    <trd>landen</trd>,
    <trd>aanleggen<klr tip="amb"> (van boot)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dobija&cacute; do <ind>brzegu</ind></trd>,
    <trd>przybija&cacute; do <ind>brzegu</ind></trd>,
    <trd>przybija&cacute; do <ind>nadbrze&zdot;a</ind></trd>,
    <trd>l&aogonek;dowa&cacute;<klr> na brzegu</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>acostar</trd>,
    <trd>chegar <klr>(&agrave; margem, costa)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_ch;&c_a;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_s;&c_t;&c_a;&c_t;&c_mol; <klr>(&c_k;
    &c_b;&c_je;&c_r;&c_je;&c_g;&c_u;)</klr></trd>
  </trdgrp>
  <trd lng="sk">prist&aacute;&tcaron;</trd>
</drv>

<drv mrk="bord.al0igxejo">
  <kap>al<tld/>i&gcirc;ejo</kap>
  <snc mrk="bord.al0igxejo.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      <ref tip="super" cel="lok.0o.spaca">Loko</ref> aran&gcirc;ita a&ubreve; ta&ubreve;ga por la
      al<tld/>i&gcirc;o de &scirc;ipoj:
      <ekz>
        <nom>Zebulun</nom> lo&gcirc;os &ccirc;e la bordo de maro, &ccirc;e
        al<tld/>i&gcirc;ejo de &scirc;ipoj
        <fnt><bib>MT</bib>, <lok>&Gen; 49:13</lok></fnt>;
      </ekz>
      <ekz>
        <nom>Alice</nom> atendis lin apud ponteto de l&#8217; boatal<tld/>i&gcirc;ejo
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la tiea al<tld/>i&gcirc;ejo ni trovis kelkajn boatojn
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="https://tekstaro.com/t?nomo=artikoloj-el-monato"
              >Venezuelo &dash; Inter maro kaj arbarego</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ankr.0ejo">ankrejo</ref>,
      <ref cel="sxip.el0ejo">el&scirc;ipejo</ref>,
      <ref cel="kaj1.0o.MAR">kajo<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_ch;&c_a;&c_l;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_s;&c_t;&c_a;&c_n;&c_mol;</trd>
  </trdgrp>
  <trd lng="es">atracadero</trd>
  <trdgrp lng="fr">
    <trd>mouillage<klr tip="ind"> (lieu d'ancrage)</klr></trd>,
    <trd>quai</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>aanlegplaats</trd>,
    <trd>steiger</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przysta&nacute;</trd>,
    <trd>keja</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>cais</trd>,
    <trd>p&iacute;er</trd>,
    <trd>atracadouro</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_s;&c_t;&c_a;&c_n;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_ch;&c_a;&c_l;</trd>
  </trdgrp>
  <trd lng="sk">pr&iacute;stav</trd>
</drv>

<drv mrk="bord.cxirkaux0o">
  <kap>&ccirc;irka&ubreve;<tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      <tld lit="B"/>o, kiu &ccirc;irka&ubreve;as lokon:
      <ekz>
        <klr>[insuletoj,]</klr> kies &ccirc;irka&ubreve;<tld/>o longas
        je proksimume unu duonle&ubreve;go
        <fnt><bib>Revadoj</bib>, <lok>kvina promenado</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">rive<klr tip="ind"> (circulaire)</klr></trd>
</drv>

<drv mrk="bord.de0igxi">
  <kap>de<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="bord.de0igxi.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Malproksimi&gcirc;i de la <tld/>o, forlasi la <tld/>on,
      na&gcirc;ante a&ubreve; &scirc;ipante:
      <ekz>
        la vapor&scirc;ipo malrapide de<tld/>i&gcirc;as
        <fnt><bib>Metrop</bib>, <lok>&ccirc;apitro 10a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_p;&c_l;&c_y;&c_c;&c_mol; &c_a;&c_d;
  <ind>&c_b;&c_je;&c_r;&c_a;&c_g;&c_a;</ind> <klr tip="amb">(&c_p;&c_r;&c_a; 
  &c_k;&c_a;&c_r;&c_a;&c_b;&c_je;&c_l;&c_mol;)</klr></trd>
  <trdgrp lng="fr">
    <trd>appareiller<klr tip="ind"> (quitter le mouillage)</klr></trd>,
    <trd>s'<ind>&eacute;loigner</ind><klr tip="amb"> (de la
    rive)</klr></trd>,
    <trd>quitter le mouillage</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>afleggen<klr tip="amb"> (van boot)</klr></trd>,
    <trd>in zee steken</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>odbija&cacute; od <ind>brzegu</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_ch;&c_a;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_p;&c_l;&c_y;&c_t;&c_mol; &c_o;&c_t;
    &c_b;&c_je;&c_r;&c_je;&c_g;&c_a;<klr tip="amb"> (&c_o;
    &c_k;&c_o;&c_r;&c_a;&c_b;&c_l;&c_je;)&gt;</klr></trd>
  </trdgrp>
</drv>

<drv mrk="bord.mar0o">
  <kap>mar<tld/>o</kap>
  <snc mrk="bord.mar0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <tld lit="B"/>o, kiu randas maron:
      <ekz>
        &ccirc;e la tagi&gcirc;o Jesuo staris sur la mar<tld/>o
        <fnt><bib>NT</bib>, <lok>&Joh; 21:4</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="marsk.0o">marsko</ref>
  </snc>
  <trd lng="bg">&c_k;&c_r;&c_a;&c_j;&c_b;&c_r;&c_je;&c_zh;&c_i;&c_je;</trd>
  <trd lng="cs">mo&rcaron;sk&eacute; pob&rcaron;e&zcaron;&iacute;</trd>
  <trd lng="de">K&uuml;ste</trd>
  <trd lng="en">coast</trd>
  <trd lng="es">costa</trd>
  <trd lng="fr">c&ocirc;te<klr tip="amb"> (marine)</klr></trd>
  <trd lng="nl">kust</trd>
  <trdgrp lng="pl">
    <trd>brzeg morski</trd>,
    <trd>brzeg morza</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_s;&c_k;&c_o;&c_j;
  <ind>&c_b;&c_je;&c_r;&c_je;&c_g;</ind></trd>
  <trd lng="sk">morsk&eacute; pobre&zcaron;ie</trd>
</drv>

<drv mrk="bord.super0igxi">
  <kap>super<tld/>i&gcirc;i,
  <var><kap>trans<tld/>i&gcirc;i</kap></var></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Elver&scirc;i&gcirc;i super la <tld/>o, superakvi la <tld/>on:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        super<tld/>i&gcirc;is en ilia koro la intencoj
        <fnt><bib>MT</bib>, <lok>&Psa; 73:7</lok></fnt>;
      </ekz>
      <ekz>
        la impeto de la pluvotorento kun&scirc;iris la ponton
        kaj la akvo trans<tld/>i&gcirc;is
        <fnt>
          <aut>M. J&oacute;kai, trad. J. Horvath</aut>
          <vrk>La du salikoj de Nagyenyed</vrk>
          <lok>2002</lok>
        </fnt>.
      </ekz>
