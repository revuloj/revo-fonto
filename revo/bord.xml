<drv mrk="bord.super0igxi">
  <kap>super<tld/>i&gcirc;i,
  <var><kap>trans<tld/>i&gcirc;i</kap></var></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Elver&scirc;i&gcirc;i super la <tld/>o, superakvi la <tld/>on:
      <ekz>
        la impeto de la pluvotorento kun&scirc;iris la ponton
        kaj la akvo trans<tld/>i&gcirc;is
        <fnt>
          <aut>M. J&oacute;kai, trad. J. Horvath</aut>
          <vrk>La du salikoj de Nagyenyed</vrk>
          <lok>2002</lok>
        </fnt>;
      </ekz>
      <ekz>
        la rivero <klr>[&#8230;]</klr> sporade trans<tld/>i&gcirc;is kaj inundis la &ccirc;irka&ubreve;a&jcirc;on
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2016/011542.php">La kimra lingvo en Patagonio</url></vrk>
          <bib>Monato</bib>
          <lok>2016/01, p. 20</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        super<tld/>i&gcirc;is en ilia koro la intencoj
        <fnt><bib>MT</bib>, <lok>&Psa; 73:7</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la situacio en Tibeto povas super<tld/>i&gcirc;i al la aliaj regionoj <klr>[&#8230;]</klr>
        tiuj de la ujguroj en &Scirc;in&gcirc;jiango, de la mongoloj en Interna Mongolio
        <fnt>
          <bib>MD08</bib>
          <vrk>Pekino en la kaptilo inter Tibeto kaj la &leftquot;internacia komunumo&rightquot;</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="inund.0i">inundi</ref>,
      <ref cel="akv.super0i">superakvi</ref>,
      <ref cel="flu.super0a">superflua</ref>,
      <ref cel="salt.trans0i">transsalti</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_j;&c_s;&c_mol;&c_c;&c_ib; &c_z;&c_mol; <ind>&c_b;&c_je;&c_r;&c_a;&c_g;&c_o;&c_w;</ind></trd>
  <trd lng="de">&uuml;ber das <ind>Ufer</ind> treten</trd>
  <trd lng="fr">d&eacute;border <klr tip="ind">(en p. d'une
  rivi&egrave;re)</klr></trd>
  <trd lng="es">desbordarse</trd>
  <trdgrp lng="id">
    <trd><baz>limpah</baz>melimpah</trd>,
    <trd><baz>luap</baz>meluap</trd>,
    <trd>luber</trd>,
    <trd><baz>luber</baz>meluber</trd>,
    <trd><baz>ruap</baz>meruap</trd>,
    <trd><baz>lembak</baz>melembak</trd>
  </trdgrp>
  <trd lng="ja">&#27710;&#28651;&#12377;&#12427;  <pr>&#12399;&#12435;&#12425;&#12435;&#12377;&#12427;</pr></trd>
  <trdgrp lng="pl">
    <trd>wyst&eogonek;powa&cacute; z <ind>brzeg&oacute;w</ind></trd>,
    <trd>przelewa&cacute; si&eogonek; przez <ind>brzeg</ind></trd>
  </trdgrp>
  <trd lng="pt">transbordar</trd>
  <trd lng="ru">&c_v;&c_y;&c_j;&c_t;&c_i; &c_i;&c_z;
  <ind>&c_b;&c_je;&c_r;&c_je;&c_g;&c_o;&c_v;</ind></trd>
</drv>

<drv mrk="bord.sur0igxi">
  <kap>sur<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Ekiri sur <tld/>on, ekzemple el&scirc;ipi&gcirc;inte:
      <ekz>
        kiam li sur<tld/>i&gcirc;is, renkontis lin el la urbo viro,
        havanta demonojn
        <fnt><bib>NT</bib>, <lok>&Luk; 8:27</lok></fnt>;
      </ekz>
      <ekz>
        homoj, armitaj per &scirc;ildoj kaj mallongaj lancoj, sur<tld/>i&gcirc;is kaj stari&gcirc;is en du
        vicojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        li kriis al ni en sia sensignifa lingva&ccirc;o, kaj indikis per gestoj, ke venis la
        momento por sur<tld/>i&gcirc;i
        <fnt>
          <aut>Anna L&ouml;wenstein</aut>
          <vrk>La &scirc;tona urbo</vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="ter.al0igxi.MAR">alteri&gcirc;i<sncref/></ref>
  </snc>
  <trd lng="be">&c_s;&c_h;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol; &c_n;&c_a; <ind>&c_b;&c_je;&c_r;&c_a;&c_g;</ind></trd>
  <trd lng="de">von <ind>Bord</ind> gehen</trd>
  <trd lng="fr">toucher terre</trd>
  <trdgrp lng="id">
    <trd>turun <klr>(dr kapal dsb.)</klr></trd>,
    <trd><baz>sentuh</baz>menyentuh tanah</trd>
  </trdgrp>
  <trd lng="ja">&#19978;&#38520;&#12377;&#12427;  <pr>&#12376;&#12423;&#12358;&#12426;&#12367;&#12377;&#12427;</pr></trd>
  <trdgrp lng="pl">
    <trd>schodzi&cacute; na <ind>brzeg</ind></trd>,
    <trd>schodzi&cacute; na <ind>l&aogonek;d</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_s;&c_a;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja;
    &c_n;&c_a; &c_b;&c_je;&c_r;&c_je;&c_g;</trd>,
    <trd>&c_s;&c_o;&c_j;&c_t;&c_i; &c_n;&c_a;
    &c_b;&c_je;&c_r;&c_je;&c_g;</trd>
  </trdgrp>
</drv>

<drv mrk="bord.Lazura_0o">
  <kap>
    <ofc>10</ofc>Lazura <tld lit="B"/>o, <var><kap>Lazura Mar<tld/>o</kap></var>
  </kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      La franca mediteranea <tld/>o inter la urboj <nom>Cassis</nom> &ccirc;e Marsejlo kaj
      <nom>Menton</nom>:
      <ekz>
        li anka&ubreve; promesis testamenti al mi vastan domon kaj terenon, kiujn li posedas &ccirc;e
        la Lazura Mar<tld/>o
        <fnt>
          <bib>CKv</bib>
          <lok>7</lok>
        </fnt>;
      </ekz>
      <ekz>
        membrokunveno de <nom>UFE</nom> <klr>[&#8230;]</klr> okazis en <nom>Boulouris</nom> apud
        <nom>Saint Rapha&euml;l</nom>
        (Lazura Mar<tld/>o)
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 7 (69)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_B;&c_l;&c_a;&c_k;&c_ib;&c_t;&c_n;&c_y; <ind>&c_B;&c_je;&c_r;&c_a;&c_g;</ind></trd>
  <trdgrp lng="de">
    <trd>C&ocirc;te d&#8217;Azur</trd>,
    <trd>Franz&ouml;sische Riviera</trd>
  </trdgrp>
  <trd lng="en">French Riviera</trd>
  <trd lng="fr">C&ocirc;te d'Azur</trd>
  <trd lng="ru">&c_L;&c_a;&c_z;&c_u;&c_r;&c_n;&c_y;&c_j; &c_B;&c_je;&c_r;&c_je;&c_g;</trd>
</drv>

