    <dif>
      <ref tip="vid" cel="sinus1.0o.ANA">Sinus</ref><ref tip="lst" cel="inflam.0o" lst="voko:malsanoj">inflamo</ref>:
      <ekz>
        mi rajtis nur unu naztukon <klr>...</klr> semajne, kaj &ccirc;ar jam 
        de infana&gcirc;o mi havis <tld/>on, tio estis tute ne sufi&ccirc;a
        <fnt>
          <aut>A. LeQuint</aut>,
          <vrk>Infantraviva&jcirc;oj</vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">sinusitis</trd>
  <trd lng="cs">sinusitida</trd>
  <trdgrp lng="de">
    <trd>Sinusitis</trd>,
    <trd>Nasennebenh&ouml;hlenentz&uuml;ndung</trd>
  </trdgrp>
  <trd lng="fr">sinusite</trd>
  <trd lng="id">sinusitis</trd>
  <trd lng="sk">sinusit&#xED;da</trd>
  <trd lng="uk">&#x441;&#x438;&#x43D;&#x443;&#x441;&#x438;&#x442;</trd>
</drv>
</art>

