<kap>
   <rad>Inuktitut</rad>/o <fnt><bib>Viki</bib></fnt>
</kap>

<drv mrk="inukti.0o">
   <kap><tld/>o</kap>
   <mlg kod="ISO-639">iu</mlg>
   <snc mrk="inukti.0o.LIN">
     <uzo tip="fak">LIN</uzo>
     <dif>
       <ref tip="super" cel="inuit.0a">Inuita</ref>
       <ref tip="lst" cel="lingv.0o" lst="voko:lingvoj">lingvo</ref>
       parolata en <ref cel="kanad.K0o">Kanado</ref>
       kaj unu el la oficialaj lingvoj de Nunavuto:
       <ekz>
         studentoj estis varbitaj por kantokurso en
         <tld/>a lingvo
         <fnt>
           <aut>E. Avila, trad. Herman D.</aut>
           <vrk><url ref="https://eo.globalvoices.org/2018/03/2857">
           Reludversioj de popkantoj por akceli la lingvon
           inuktitutan</url></vrk>
           <lok>GlobalVoices, 2018-03-05</lok>
         </fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_ib;&c_n;&c_u;&c_k;&c_t;&c_y;&c_t;&c_u;&c_t;</trd>
   <trd lng="de">Inuktitut</trd>
   <trd lng="en">Inuktitut</trd>
   <trd lng="fr">inuktitut</trd>
   <trd lng="iu">&#5123;&#x14C4;&#5251;&#x144E;&#5200;&#5222;</trd>
   <trd lng="nl">Inuktitut</trd>
   <trd lng="pl">Inuktitut</trd>
   <trdgrp lng="ru">
     <trd>&c_I;&c_n;&c_u;&c_k;&c_t;&c_i;&c_t;&c_u;&c_t;</trd>,
     <trd>&c_I;&c_n;&c_u;&c_k;&c_t;&c_i;&c_t;&c_u;&c_t; &c_ja;&c_z;&c_y;&c_k;</trd>
   </trdgrp>
  <trd lng="zh">&#x56E0;&#x7D10;&#29305;&#x8A9E; <pr>y&#x12B;nni&#x1D4;t&#xE8;y&#x1D4;</pr></trd>
</drv>
