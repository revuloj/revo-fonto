<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gaded.xml,v 1.12 2024/01/22 09:26:33 revo Exp $">
<kap>
  <rad>gaded</rad>/oj
</kap>
<drv mrk="gaded.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt" cel="gad.0oformaj">gadoformaj
      fi&scirc;oj</ref> (<trd lng="la">Gadidae</trd>):
      <ekz>
        la polusa <ref tip="prt" cel="gad.0o">gado</ref>
        apartenas al la familio de <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=46806">Polusa gado</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
       <ref cel="eglefi.0o">eglefino</ref>,
       <ref cel="merlan.0o">merlango</ref>,
       <ref cel="lojt.0o">lojto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_a;&c_s;&c_k;&c_o;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  <trd lng="ca">g&agrave;dids</trd>
  <trd lng="de">Dorsche<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="fr">gadid&eacute;s</trd>
  <trd lng="hu">t&odblac;kehalf&eacute;l&eacute;k</trd>
  <trd lng="nl">dorsen<klr tip="ind"> (fam.)</klr></trd>
  <trdgrp lng="pt">
    <trd>gad&iacute;deos</trd>,
    <trd>g&aacute;didas</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_s;&c_k;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&c_t;&c_r;&c_ib;&c_s;&#x43A;&#x43E;&c_v;&c_ib;</trd>
</drv>
</art>
