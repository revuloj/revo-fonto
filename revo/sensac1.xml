<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>sensaci</rad>/o
</kap>
<drv mrk="sensac1.0o">
  <kap><tld/>o</kap>
  <snc mrk="sensac1.0o.baz">
    <dif>
      Forta impreso sur publikon:
      <ekz>
        estis <tld/>o, kiam, post prilukto de grandegaj malfacila&jcirc;oj, en
        la E&ubreve;ropa Centro de Nuklea Esploro CERN, &Gcirc;enevo,
en 1995 oni
        sukcesis kunmeti kompletan antiatomon<fnt><aut>OH</aut>: <vrk>&Ccirc;u
        antimaterio en la universo?</vrk>, <bib>Monato</bib><lok>jaro
        2001a, numero 2a, p. 23a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="impon.0eco">imponeco</ref>
    </refgrp>
    <trd lng="be">&c_s;&c_e;&c_n;&c_s;&c_a;&c_c;&c_y;&c_ja;</trd>
    <trd lng="de">Sensation</trd>
    <trdgrp lng="en">
      <trd>sensation</trd>,
      <trd>impression</trd>,
      <trd>commotion</trd>,
      <trd>interest</trd>,
      <trd>furor</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>sensation<klr> (effet spectaculaire)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>szenz&aacute;ci&oacute;</trd>,
      <trd>felt&udblac;n&eacute;s</trd>
    </trdgrp>
    <trd lng="nl">sensatie</trd>
    <trd lng="ru">&c_s;&c_je;&c_n;&c_s;&c_a;&c_c;&c_i;&c_ja;</trd>
  </snc>
  <snc mrk="sensac1.0o.fil">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Spirita impreso ricevita per iu el la kvin sentumoj.
    </dif>
    <refgrp tip="sin">
      <ref cel="sensac.0o">sensaco</ref>
    </refgrp>
    <trd lng="be">&c_u;&c_s;&c_p;&c_r;&c_y;&c_m;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
    <trdgrp lng="en">
      <trd>perception</trd>,
      <trd>sensation</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>perception</trd>,
      <trd>sensation</trd>
    </trdgrp>
    <trd lng="hu">&eacute;rzet</trd>
    <trd lng="ru">&c_v;&c_o;&c_s;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_i;&c_je;</trd>
  </snc>
  <trdgrp lng="de">
    <trd>Empfindung</trd>,
    <trd>Sinneswahrnehmung</trd>
  </trdgrp>
  <trd lng="nl">zintuiglijke <ind>waarneming</ind></trd>
</drv>
</art>
