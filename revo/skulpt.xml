<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: skulpt.xml,v 1.23 2017/12/20 14:10:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>skulpt</rad>/i
</kap>
<drv mrk="skulpt.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="skulpt.0i.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Formi figurojn el solida materio, kiel &scirc;tono, marmoro, ligno,
      argilo, vakso, a&ubreve; hakante, a&ubreve; tran&ccirc;ante, a&ubreve;
      skrapante, a&ubreve; knedante:
      <ekz>
        <tld/>i statuon;
      </ekz>
      <ekz>
        <tld/>arto;
      </ekz>
      <ekz>Se mi povus <tld/>i, mi jam delonge estus farinta lian
      bronzan buston
        <fnt>
          <aut>Willy Wood, trad. Willy van Zoest</aut>
          <vrk>Blua sango</vrk>
          <lok>&ccirc;apitro 14a</lok>
        </fnt>;
      </ekz>
      <ekz>li <tld/>is du &scirc;tonajn tabelojn kiel la unuaj
        <fnt><bib>MT</bib><lok>&Eli; 34:4</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ja;&c_p;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_b;&c_ja;&c_r;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_s;&c_ja;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">esculpir</trd>
  <trdgrp lng="cs">
    <trd>tesat</trd>,
    <trd>vytesat</trd>,
    <trd>vy&rcaron;ezat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schnitzen</trd>,
    <trd>ausschnitzen</trd>,
    <trd>ausstechen</trd>,
    <trd>aushauen</trd>
  </trdgrp>
  <trd lng="en">sculpture</trd>
  <trd lng="es">esculpir</trd>
  <trd lng="fr">sculpter</trd>
  <trdgrp lng="hu">
    <trd>farag</trd>,
    <trd>kiform&aacute;z</trd>,
    <trd>(szobrot) <ind>k&eacute;sz&iacute;t</ind></trd>
  </trdgrp>
  <trd lng="nl">beeldhouwen</trd>
  <trd lng="pl">rze&zacute;bi&cacute;</trd>
  <trdgrp lng="pt">
    <trd>esculpir</trd>,
    <trd>talhar</trd>,
    <trd>entalhar</trd>,
    <trd>cinzelar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_a;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_l;&c_je;&c_p;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_s;&c_je;&c_k;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vytv&aacute;ra&tcaron; sochu</trd>,
    <trd>zhotovova&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="skulpt.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="skulpt.0ajxo.BELA">
    <dif>
      Figuro kreita per <tld/>ado:
      <ekz>
        sed &gcirc;i estas <tld/>a&jcirc;o, oni ne povas meti &gcirc;in sur muron
        <fnt>
        <aut>Machado de Assis, trad. Paulo Viana</aut>,
        <vrk>La divenistino kaj aliaj rakontoj</vrk>,
        <lok>Oportuno, jaro 2005a, p. 36a</lok>
        </fnt>;
      </ekz>
      <ekz>ne faru al vi ian <tld/>a&jcirc;on, bildon de ia idolo
        <fnt><bib>MT</bib><lok>&Rea; 4:16</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">escultura</trd>
  <trd lng="fr">sculpture<klr> (&oelig;uvre)</klr></trd>
  <trd lng="pl">rze&zacute;ba</trd>
  <trd lng="pt">escultura<klr tip="ind"> (obra de escultura)</klr></trd>
</drv>

<drv mrk="skulpt.0ilo">
  <kap><ofc>*</ofc><tld/>ilo <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url
  ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5201">skulpt'il'</url></lok></fnt></kap>
  <snc mrk="skulpt.0ilo.BELA">
    <dif>
      &Ccirc;izilo por tran&ccirc;formi malmolajn materiojn:
    </dif>
      <ekz>ni devus esti havintaj ian specon de <tld/>ilo <klr>[&ccirc;ar
      nur]</klr> per tran&ccirc;ilo &ccirc;i tio estas sklavlaboro
        <fnt>
          <aut>Astrid Lindgren, trad. Runo Stridell</aut>
          <vrk>Ronjo, rabista filino</vrk>
          <lok>&ccirc;apitro 8a</lok>
        </fnt>;
      </ekz>
      <ekz>li <klr>[...]</klr> komencis rigardi &scirc;in kun tia admiro, kun
      kia artkonanto rigardas dian statuon, verkon de majstra <tld/>ilo
        <fnt>
          <aut>Henryk Sienkiewicz, trad. Lydia Zamenhof</aut>
          <vrk>Quo vadis?</vrk>
          <lok>&ccirc;apitro 74a</lok>
        </fnt>.
      </ekz>
  </snc>
  <snc>
    <dif>
      Ilo por skrapformi a&ubreve; premformi moligitajn materiojn.
    </dif>
    <refgrp tip="vid">
      <ref cel="cxiz.0i">&ccirc;izi</ref>,
      <ref cel="modl.0i">modli</ref>
    </refgrp>
  </snc>
  <trd lng="cs">dl&aacute;to socha&rcaron;sk&eacute;</trd>
  <trd lng="de">Mei&szlig;el des Bildhauers</trd>
  <trd lng="es">cincel<klr tip="amb"> (para esculpir)</klr></trd>
  <trdgrp lng="hu">
    <trd>szobr&aacute;szv&eacute;s&odblac;</trd>,
    <trd>mint&aacute;z&oacute;k&eacute;s</trd>
  </trdgrp>
  <trd lng="nl">beitel</trd>
  <trdgrp lng="pt">
    <trd>cinzel</trd>,
    <trd>form&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_z;&c_je;&c_c;</trd>
  <trd lng="sk">dl&aacute;to soch&aacute;rske</trd>
</drv>

<drv mrk="skulpt.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="skulpt.0isto.BELA">
    <dif>
      <ref tip="super" cel="art.0isto.BELA">Artisto</ref> kiu <tld/>as:
      <ekz>
        &scirc;i estis fama <tld/>isto;
      </ekz>
      <ekz><klr>[tiu]</klr> ser&ccirc;as kompetentan <tld/>iston, por fari
      idolon fortikan
        <fnt><bib>MT</bib><lok>&Jes; 40:20</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>skulptor</trd>,
    <trd>socha&rcaron;</trd>,
    <trd>&rcaron;ezb&aacute;&rcaron;</trd>
  </trdgrp>
  <trd lng="es">escultor</trd>
  <trd lng="fr">sculpteur</trd>
  <trd lng="pl">rze&zacute;biarz</trd>
  <trd lng="pt">escultor</trd>
  <trd lng="sk">soch&aacute;r</trd>
</drv>

<drv mrk="skulpt.S0isto">
  <kap><tld lit="S"/>isto
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
  </kap>
  <mlg>Scl</mlg>
  <snc mrk="skulpt.S0isto.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Sud-hemisfera <ref tip="lst" cel="stel.0figuro"
      lst="voko:stelfiguroj">stelfiguro</ref>
      (<trd lng="la">Sculptor</trd>).
    </dif>
  </snc>
  <trd lng="en">Sculptor</trd>
  <trd lng="fr">Sculpteur<klr tip="ind"> (constellation)</klr></trd>
</drv>
</art>
