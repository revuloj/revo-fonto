<drv mrk="hor.prem0o">
  <kap>prem<tld/>o</kap>
  <snc mrk="hor.prem0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      <tld lit="H"/>o de intensa aktivado de homoj, ekzemple kiam multaj
      samtempe veturas al laborejoj, samtempe vizitas man&gcirc;ejojn
      a&ubreve; simile...:
      <ekz>
        dum matenaj kaj vesperaj prem<tld/>oj trajnoj venas &ccirc;iun 3an&#x202F;-&#x202F;5an minuton
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo/pg</aut>
          <vrk><url ref="&Monato;/2005/008674.php">Profitoj, punoj kaj privatigo</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum prem<tld/>oj veturi per biciklo a&ubreve; publika transportilo multe pli rapidas
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="&Monato;/2012/010599.php">Nova malnova trafikilo</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        en elektrouzadaj prem<tld/>oj de somero de la kuranta jaro,
        mankis preska&ubreve; 2 milionoj da <esc>kilovattoj</esc> de elektro en Tokio
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url ref="&CRI;1/2004/07/23/1@12666.htm">
          Elektro&scirc;para stato de Japanio</url></vrk>,
          <lok>2004-07-23</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Hauptverkehrszeit</trd>,
      <trd>Sto&szlig;zeit</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_s; &c_p;&c_ib;&c_k;</trd>,
    <trd>&c_g;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_a; &c_p;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="en">rush hour</trd>
  <trd lng="fr">heure de pointe</trd>
  <trd lng="id"><ind>jam</ind> sibuk</trd>
  <trd lng="it"><ind>ora</ind> di punta</trd>
  <trd lng="nl">piekuur</trd>
  <trdgrp lng="zh">
    <trd>&#x9AD8;&#x5CF0;&#x671F; <pr>g&#x101;of&#x113;ngq&#x12B;</pr></trd>,
    <trd>&#x4E0A;&#x4E0B;&#x73ED;&#x65F6;&#x95F4; <pr>sh&#xE0;ngxi&#xE0;b&#x101;nsh&#xED;ji&#x101;n</pr></trd>
  </trdgrp>
</drv>
