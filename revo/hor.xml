<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>hor</rad>/o
</kap>
<drv mrk="hor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="hor.0o.mezurunuo">
    <dif>
      Komunuza <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de
      <ref cel="temp.0o.FIZ">tempo</ref>, egala al 3600
      <ref tip="prt" cel="sekund.0o">sekundoj</ref>; simb. h:
      <ekz>
        unu <tld/>o egalas al dudekkvarono de tago;
      </ekz>
      <ekz>
        kioma <tld/>o estas
        <fnt><bib>Fab1</bib>, <lok>Galo&scirc;oj de feli&ccirc;o</lok></fnt>?;
      </ekz>
      <ekz>
        <ind>duon<tld/>o</ind> <klr>(a&ubreve;: <tld/>duono)</klr>;
        <trd lng="es">media <ind>hora</ind></trd>
        <trd lng="fr">demi-heure</trd>
        <trd lng="hu">f&eacute;l&oacute;ra</trd>
      </ekz>
      <ekz>
        <ind>kvaron<tld/>o</ind> <klr>(a&ubreve;: <tld/>kvarono)</klr>;
        <trd lng="es">cuarto de <ind>hora</ind></trd>
        <trd lng="fr">quart d'heure</trd>
        <trd lng="hu">negyed&oacute;ra</trd>
        <trd lng="nl">kwartier</trd>
      </ekz>
      <ekz>
        la <tld/>montra ombro de la sunhorlo&gcirc;o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="sv">timma</trd>
  </snc>
  <snc mrk="hor.0o.tagmomento">
    <dif>
      Difinita momento de la tago:
      <ekz>
        estis la <tld/>o, kiam la leonoj iras por trinki;
      </ekz>
      <ekz>
        oni donas por man&gcirc;i je &ccirc;iu ajn <tld/>o;
      </ekz>
      <ekz>
        je lia lasta <tld/>o
        <klr>(la momento de morto)</klr>;
      </ekz>
      <ekz>
        last<tld/>a scii&gcirc;o de gazeto
        <klr>(ricevita je lasta momento, &jcirc;us anta&ubreve; la
        presado)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_a;</trd>
  <trd lng="cs">hodina</trd>
  <trdgrp lng="de">
    <trd>Stunde</trd>,
    <trd>Uhrzeit</trd>,
    <trd>Zeit</trd>
  </trdgrp>
  <trd lng="vo">hol</trd>
  <trd lng="el">&omega_ton;&rho;&alfa;</trd>
  <trd lng="en">hour</trd>
  <trd lng="es">honra</trd>
  <trd lng="fr">heure</trd>
  <trd lng="hu">&oacute;ra<klr tip="ind"> (id&odblac;)</klr></trd>
  <trd lng="nl">uur</trd>
  <trd lng="pt">hora</trd>
  <trd lng="os">&c_s;&c_a;&c_h;&c_a;&c_t;</trd>
  <trd lng="ru">&c_ch;&c_a;&c_s;</trd>
  <trd lng="sk">hodina</trd>
</drv>

<drv mrk="hor.0aro0libro">
  <kap><tld/>aro, <tld/>libro</kap>
  <snc>
    <dif>
      Libreto, en kiu oni sciigas la <tld/>ojn, kiam okazas diversaj
      faktoj:
      <ekz>
        fervoja, tajda <tld/>aro.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">horari</trd>
  <trdgrp lng="de">
    <trd>Zeitplan</trd>,
    <trd>Fahrplan <klr>(trajnhoraro)</klr></trd> k.a.
  </trdgrp>
  <trd lng="es">horario</trd>
  <trd lng="fr">horaire</trd>
  <trd lng="hu">menetrend</trd>
  <trdgrp lng="nl">
    <trd>dienstregeling</trd>,
    <trd>spoorboekje</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_i;&c_s;&c_a;&c_n;&c_i;&c_je;</trd>
  <trd lng="sv">tidtabell</trd>
</drv>

<drv mrk="hor.0montrilo">
  <kap><tld/>montrilo</kap>
  <snc>
    <dif>
      Turni&gcirc;anta vergeto por indiki la <tld/>on
      &ccirc;e horlo&gcirc;o.
    </dif>
  </snc>
  <trd lng="de">Stundenzeiger</trd>
  <trd lng="es">manecilla</trd>
  <trdgrp lng="fr">
    <trd><ind>aiguille</ind> des heures</trd>,
    <trd>grande <ind>aiguille</ind></trd>
  </trdgrp>
  <trd lng="hu">&oacute;ramutat&oacute;</trd>
  <trd lng="nl">grote <ind>wijzer</ind>
  </trd>
  <trd lng="ru">&c_ch;&c_a;&c_s;&c_o;&c_v;&c_a;&c_ja;
  &c_s;&c_t;&c_r;&c_je;&c_l;&c_k;&c_a;</trd>
  <trd lng="sv">timvisare</trd>
</drv>

<drv mrk="hor.cxiu0e">
  <kap>&ccirc;iu<tld/>e</kap>
  <snc>
    <dif>
      Je &ccirc;iu <tld/>o:
      <ekz>
        vi devas trinki &ccirc;iu<tld/>e du kulerojn de tiu medikamento.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_t;&c_o;&c_g;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_n;&c_a;</trd>
  <trd lng="de">st&uuml;ndlich</trd>
  <trdgrp lng="hu">
    <trd>minden <ind>&oacute;r&aacute;ban</ind></trd>,
    <trd>&oacute;r&aacute;nk&eacute;nt<klr tip="ind"> (minden &oacute;r&aacute;ban)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ieder <ind>uur</ind></trd>,
    <trd>om 't <ind>uur</ind></trd>
  </trdgrp>
  <trd lng="os">&c_a;&c_l;&c_y; &c_s;&c_a;&c_h;&c_a;&c_t; &c_d;&aelig;&c_r;</trd>
  <trd lng="ru">&c_je;&c_zh;&c_je;&c_ch;&c_a;&c_s;&c_n;&c_o;</trd>
  <trd lng="sk">ka&zcaron;d&uacute; <ind>hodinu</ind></trd>
  <trd lng="sv">varje <ind>timma</ind></trd>
</drv>

<drv mrk="hor.po0e">
  <kap>po<tld/>e</kap>
  <snc>
    <dif>
      Por &ccirc;iu <tld/>o:
      <ekz>
        la seruristo postulas dek frankojn po<tld/>e.
      </ekz>
    </dif>
  </snc>
  <adm>
  Mi havas dubon, cxu tio estas konsilinda dirmaniero.
  Mi pli volonte dirus "po dek frankoj(n) hore". Sxajnas,
  ke tio estis truko por eviti la problemon de akuzativo
  post po. <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_g;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_n;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_zh;&c_n;&c_u;&c_ju;
    &c_g;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>pro <ind>Stunde</ind></trd>,
    <trd>st&uuml;ndlich</trd>
  </trdgrp>
  <trd lng="es">por <ind>hora</ind></trd>
  <trd lng="fr">par <ind>heure</ind></trd>
  <trd lng="hu">&oacute;r&aacute;nk&eacute;nt</trd>
  <trd lng="nl">per <ind>uur</ind></trd>
  <trd lng="ru">&c_v; &c_ch;&c_a;&c_s;</trd>
</drv>
</art>
