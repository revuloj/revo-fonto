<drv mrk="hor.0e">
 <kap><tld/>e</kap>
 <snc mrk="hor.0e.KOMUNE">
   <dif>
     En, por, post unu <tld/>o:
     <ekz>
       oni pagos minimume 8 e&ubreve;rojn <tld/>e
       <fnt>
       <aut>LAST</aut>, <vrk>Sklaveco en fungejo</vrk>.
       <bib>Monato</bib>
       </fnt>;
     </ekz>
      <ekz>
        <tld/>e, li revenis kun kru&ccirc;o enmane
        <fnt>
          <bib>Rob</bib>
        </fnt>;
      </ekz>
      <ekz>
        24-<tld/>e malfrua forflugo
        <fnt>
          <bib>CKv</bib>
        </fnt>;
      </ekz>
      <ekz>
        kvarfoje <tld/>e oni devis iom pli eltiri la &scirc;tofostrion
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        nemultaj klientoj venis &ccirc;i-<tld/>e
        <fnt>
          <bib>ChB</bib>
        </fnt>;
      </ekz>
     <ekz>
       veturdistanco estis inter 10 kaj 90 km, meza rapideco po
       10 km <tld/>e
       <fnt>
       <vrk>Vakeroj de Alpoj</vrk>. <bib>Monato</bib>
       </fnt>;
     </ekz>
     <ekz>
       luado de boatoj: 5 rub/<tld/>e
       <fnt>
       <vrk>La 32a Orienta Somera Esperantista Junulara Tendaro</vrk>.
       <bib>LOdE</bib>, <lok>2000. numero 5 (67)</lok>
       </fnt>.
     </ekz>
   </dif>
 </snc>
 <trdgrp lng="be">
   <trd>&c_p;&c_a;&c_g;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_n;&c_a;</trd>,
   <trd>&c_k;&c_o;&c_zh;&c_n;&c_u;&c_ju;
   &c_g;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_u;</trd>
 </trdgrp>
 <trd lng="br">dre <ind>eur</ind></trd>
 <trdgrp lng="de">
   <trd>pro <ind>Stunde</ind></trd>,
   <trd>st&uuml;ndlich</trd>
 </trdgrp>
 <trdgrp lng="en">
   <trd>hourly</trd>,
   <trd>per hour</trd>
 </trdgrp>
 <trd lng="es">por <ind>hora</ind></trd>
 <trd lng="fr">par <ind>heure</ind></trd>
 <trd lng="hu">&oacute;r&aacute;nk&eacute;nt</trd>
 <trdgrp lng="it">
   <trd>all'<ind>ora</ind></trd>,
   <trd>per <ind>ora</ind></trd>
 </trdgrp>
 <trd lng="nl">per <ind>uur</ind></trd>
