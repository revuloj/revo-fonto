  <trd lng="hu">&oacute;r&aacute;nk&eacute;nt</trd>
  <trd lng="id">per <ind>jam</ind></trd>
  <trdgrp lng="it">
   <trd>all'<ind>ora</ind></trd>,
   <trd>per <ind>ora</ind></trd>
