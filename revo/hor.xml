 <trd lng="pl">na/za <ind>godzin&eogonek;</ind></trd>
 <trd lng="pt">por <ind>hora</ind><klr tip="ind"> (adv.)</klr></trd>
 <trd lng="ru">&c_v; &c_ch;&c_a;&c_s;</trd>
</drv>

<drv mrk="hor.0aro">
  <kap><tld/>aro, <var><kap><tld/>libro</kap></var></kap>
  <snc>
    <dif>
      <ref tip="super" cel="plan.0o.realigota">Plano</ref> <ref tip="super" cel="tabel.0o.TIP">tabel</ref>forma 
      kun la la&ubreve;hore aran&gcirc;itaj tempoj de servo, natura fenomeno ks;
      <ref tip="sin" cel="tabel.hor0o">hortabelo</ref>:
      <ekz>
        fervoja, tajda <tld/>aro;
      </ekz>
      <ekz>
        <ind>lerneja <tld/>aro</ind>
        <fnt>
          <bib>EeP</bib>
        </fnt>;
        <trd lng="de">Stundenplan</trd>
      </ekz>
      <ekz>
        kelkaj fervojaj kompanioj enmetis esperantlingvajn tekstojn en la porpasa&gcirc;erajn
        klarigojn de siaj <tld/>aroj
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#8230;]</klr> profundi&gcirc;is en la misterojn de la <ind>bus-<tld/>aro</ind>j, afi&scirc;itaj en la
        halo
        <fnt>
          <bib>ChR</bib>
        </fnt>;
        <trd lng="de">Busfahrplan</trd>
      </ekz>
      <ekz>
        la <ind>televida <tld/>aro</ind> principe ne &scirc;an&gcirc;i&gcirc;os, &rightquot;Propra ludo&#8221; plu aperados diman&ccirc;e
        posttagmeze
        <fnt>
          <bib>LOdE</bib>
          <vrk><url ref="https://tekstaro.com/t?nomo=ondo-de-esperanto"
              >En &leftquot;Propra ludo&rightquot; dum la tuta jaro</url></vrk>
        </fnt>.
        <trd lng="de">Fernsehprogramm</trd>
      </ekz>
   </dif>
    <refgrp tip="vid">
       <ref cel="progra.0o.KOMUNE">programo<sncref/></ref>
    </refgrp>
 </snc>
 <trd lng="br">euriadur</trd>
 <trd lng="ca">horari</trd>
 <trdgrp lng="de">
   <trd>Zeitplan</trd>,
   <trd>Fahrplan</trd>
 </trdgrp>
 <trdgrp lng="en">
   <trd>schedule</trd>,
   <trd>timetable</trd>
 </trdgrp>
 <trd lng="es">horario</trd>
 <trd lng="fr">horaire</trd>
 <trd lng="hu">menetrend</trd>
 <trd lng="it">orario<klr> (libretto)</klr></trd>
 <trdgrp lng="nl">
