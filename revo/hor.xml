  <trd lng="nl">grote <ind>wijzer</ind></trd>
  <trd lng="pl">wskaz&oacute;wka <klr>(zegara)</klr></trd>
  <trd lng="pt">ponteiro<klr tip="ind"> (de rel&oacute;gio)</klr></trd>
  <trd lng="ru">&c_ch;&c_a;&c_s;&c_o;&c_v;&c_a;&c_ja;
