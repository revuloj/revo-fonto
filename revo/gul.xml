<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gul</rad>/o
</kap>
<drv mrk="gul.0o">
  <kap><tld/>o</kap>
  <snc mrk="gul.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="mustel1.0oj">musteledoj</ref>
      (<trd lng="la">Gulo</trd>), mezgranda raba besto de Norda Azio
      kun bruna felo, vivanta apud mar&ccirc;oj a&ubreve; en arbaroj.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_a;&c_m;&c_a;&c_h;&c_a;</trd>
  <trd lng="cs">rosom&aacute;k</trd>
  <trd lng="de">Vielfra&szlig;</trd>
  <trd lng="fr">glouton<klr tip="amb"> (animal)</klr></trd>
  <trdgrp lng="hu">
    <trd>rozsom&aacute;k</trd>,
    <trd>torkos <ind>borz</ind></trd>
  </trdgrp>
  <trd lng="nl">veelvraten</trd>
  <trdgrp lng="pt">
    <trd>glut&#227;o <klr tip="amb">(zool.)</klr></trd>,
    <trd>carcaju</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_o;&c_s;&c_o;&c_m;&c_a;&c_h;&c_a;</trd>
</drv>
</art>
