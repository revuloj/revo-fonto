<drv mrk="sond.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sond.0i.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="esplor.0i">Esplori</ref> per aparato a&ubreve; nerekta metodo ne facile
      penetreblan
      fizikan medion, ekzemple akvon sub &scirc;ipo, terinternon, vivulan korpon:
      <ekz>
        la maristoj eksentis &ccirc;irka&ubreve; noktomezo, ke ili alproksimi&gcirc;as al ia lando kaj
        <tld/>inte, ili trovis dudek klaftojn
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 27:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj okuloj penadis <tld/>i la senlumecon, &ccirc;iumomente &scirc;i kredis, ke nigra
        profilo aperas
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi volas <tld/>i tiun merkaton por la kuiriloj
        <fnt>
          <bib>KrM</bib>
          <lok>6. Kiu Ne Riskas, Tiu Ne Gajnas.</lok>
        </fnt>;
      </ekz>
      <ekz>
        li palpser&ccirc;is remilon, <tld/>is <klr>[&#x2026;]</klr> la rokon
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 46</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="detekt.0i">detekti</ref>,
      <ref cel="ekzame.0i.esplori">ekzameni<sncref/></ref>,
      <ref cel="inspek.0i">inspekti</ref>,
      <ref cel="penetr.0i">penetri</ref>,
      <ref cel="sens.0i">sensi</ref>
    </refgrp>
    <trd lng="en">probe</trd>
    <trd lng="hu">szond&aacute;z</trd>
    <trd lng="nl">peilen</trd>
  </snc>
  <snc mrk="sond.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Prove <ref tip="super" cel="esplor.0i">esplori</ref>, nerekte kolekti informojn pri malfacile
      alirebla
      homa medio:
      <ekz>
        mi <tld/>as &ccirc;e la kortego
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni ne anta&ubreve;ju&gcirc;u, oni devas anta&ubreve;e <tld/>i la princon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i la homan koron
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        la virinoj <klr>[&#x2026;]</klr> avidis <tld/>i la misteron, &ccirc;irka&ubreve;antan la belan
        fremdulon
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        se ni nur kura&gcirc;as <tld/>i la profundon de nia
        vivangoro
        <fnt>
          <aut>S. Johansson</aut>,
          <vrk>Epokfara metamorfozo</vrk>,
          <bib>Monato</bib>,
          <lok>1997:6, p. 14a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="prov.0i.elmeti_iun_al_pritaksado">provi<sncref/></ref>,
      <ref cel="tent.0i.provi">tenti<sncref/></ref>
    </refgrp>
    <trd lng="de">eruieren</trd>
    <trd lng="en">probe</trd>
    <trdgrp lng="hu">
      <trd>kipuhatol</trd>,
      <trd>kif&uuml;rk&eacute;sz</trd>
    </trdgrp>
    <trd lng="nl">polsen</trd>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_n;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>p&aacute;trat</trd>,
    <trd>sondovat</trd>,
    <trd>zkoumat</trd>
  </trdgrp>
  <trdgrp lng="de">
