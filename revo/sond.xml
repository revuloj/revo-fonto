<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>sond</rad>/i
</kap>
<drv mrk="sond.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Mezuri per tiucela aparato la profundecon de akvo a&ubreve; grundo.
    </dif>
    <trd lng="nl">peilen</trd>
  </snc>
  <snc>
    <dif>
      Esplori per tiucela aparato la fundon de akvo a&ubreve; de grundo.
    </dif>
    <trd lng="nl">peilen</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Prove esplori:
      <ekz>
        mi <tld/>as &ccirc;e la kortego<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i la opinion.
      </ekz>
    </dif>
    <trd lng="nl">polsen</trd>
  </snc>
  <trd lng="de">peilen</trd>
  <trdgrp lng="en">
    <trd>sound</trd>,
    <trd>fathom</trd>,
    <trd>plumb</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_o;&c_n;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
