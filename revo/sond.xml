<drv mrk="sond.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="sond.0ajxo.KOMUNE">
    <dif>
      Rezulto de <tld/>o:
      <ekz>
        dum en la lasta balotado la diversaj <tld/>a&jcirc;oj
        &gcirc;uste prognozis la rezulton, &ccirc;i-foje ili maltrafis
        <fnt>
          <aut>K. Juh&aacute;sz</aut>,
          <vrk>Apena&ubreve; venko</vrk>,
          <bib>Monato</bib>,
          <lok>2002:6, p. 12a</lok>
        </fnt>;
      </ekz>
      <ekz>
        tuja postbalota <tld/>a&jcirc;o indikis, ke klare venkos la konservativa partio
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2015/011508.php">Malpli malbonaj inter aliaj malbonaj</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        opinio-<tld/>a&jcirc;oj en diversaj e&ubreve;ropaj landoj <klr>[&#x2026;]</klr> indikis, ke
        civitanoj <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2013/010967.php">Referendumo ena-ekstera</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_n;&c_ib;&c_k;
  <ind>&c_z;&c_a;&c_n;&c_d;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ja;</ind></trd>
  <trdgrp lng="de">
    <trd>Umfrage</trd>,
    <trd>Sondierung</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>result of probing</trd>,
    <trd>survey</trd>
  </trdgrp>
  <trd lng="es">resultado de un <ind>sondeo</ind></trd>
  <trd lng="fr">r&eacute;sultat de <ind>sondage</ind></trd>
  <trd lng="hu">kutat&aacute;si eredm&eacute;ny</trd>
  <trd lng="pl">sonda&zdot;</trd>
  <trd lng="ru">&c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_t;
  <ind>&c_z;&c_o;&c_n;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;&c_ja;</ind></trd>
  <trdgrp lng="zh">
    <trd>&#x8ABF;&#x67E5; <pr>di&#xE0;och&#xE1;</pr></trd>,
    <trd>&#x8C03;&#x67E5; <pr>di&#xE0;och&#xE1;</pr></trd>,
    <trd>&#x6C11;&#x610F;&#x8ABF;&#x67E5; <pr>m&#xED;ny&#xEC;di&#xE0;och&#xE1;</pr></trd>,
    <trd>&#x6C11;&#x610F;&#x8C03;&#x67E5; <pr>m&#xED;ny&#xEC;di&#xE0;och&#xE1;</pr></trd>,
