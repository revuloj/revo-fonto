    <trd>&#x6E2C;&#x6DF1;&#x3059;&#x308B;  <pr>&#x305D;&#x304F;&#x3075;&#x304B;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x691C;&#x67FB;&#x3059;&#x308B;  <pr>&#x3051;&#x3093;&#x3055;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x6253;&#x8A3A;&#x3059;&#x308B;  <pr>&#x3060;&#x3057;&#x3093;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>sondowa&cacute;</trd>,
    <trd>bada&cacute;</trd>,
    <trd>zg&lstroke;&eogonek;bia&cacute;</trd>,
    <trd>gruntowa&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_o;&c_n;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
