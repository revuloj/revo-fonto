<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: balg.xml,v 1.21 2025/10/28 12:59:09 revo Exp $">
<kap>
  <rad>balg</rad>/o
  <fnt><bib>V</bib></fnt>
</kap>
<drv mrk="balg.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc mrk="balg.0o.TEK">
    <dif>
      Regule zigzage refaldita ledo, tolo k. s. kunliganta du moveblajn
      partojn de iu aparato tiel, ke oni povas ilin proksimigi a&ubreve;
      malproksimigi:
      <ekz>
        <tld/>o de fotografaparato, de fajro<ref cel="blov.0ilo">blovilo</ref>;
      </ekz>
      <ekz>
        intervagona <tld/>o;
      </ekz>
      <ekz>
        akordionoj, koncertinoj, bandonioj <klr>[&#x2026;]</klr> funkcias per <tld/>o premita
        inter la manoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=69745"
              >Bu&scirc;harmoniko</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>otretanto <klr>[&#x2026;]</klr> garantiis al orgenoj la provizadon de la
        instrumento per aero
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=229693"
              >Balgotretanto</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sak.blov0o">blovsako</ref>
    </refgrp>
  <trdgrp lng="de">
    <trd>Balg</trd>,
    <trd>Faltenbalg</trd>
  </trdgrp>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_h; <klr>(&c_n;&c_a;&c_p;&c_r;. &c_g;&c_a;&c_r;&c_m;&c_o;&c_n;&c_ib;&c_k;&c_a;, &c_k;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_s;&c_k;&c_ib;)</klr></trd>
  <trd lng="bg">&c_d;&c_u;&c_h;&c_a;&c_l;&c_o;</trd>
  <trd lng="bo">&t_sa;&t_bata;&t_gigu;&t_da;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>fouka&ccaron;ka</trd>,
    <trd>m&ecaron;ch <klr>(harmoniky, fotoapar&aacute;tu, kov&aacute;&rcaron;sk&yacute; aj.)</klr></trd>
  </trdgrp>
  <trd lng="en">bellows</trd>
  <trd lng="es">fuelle</trd>
  <trd lng="fr">soufflet <klr tip="ind">(d'accord&eacute;on)</klr></trd>
  <trd lng="he">&mem;&pe;&vav;&het;</trd>
  <trdgrp lng="hu">
    <trd>harmonika<klr tip="ind"> (red&odblac;ny)</klr></trd>,
    <trd>l&eacute;gharmonika</trd>,
    <trd>kihuzat<klr tip="ind">
    (f&eacute;nyk&eacute;pez&odblac;g&eacute;pen)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x3058;&#x3083;&#x3070;&#x3089;</trd>
  <trd lng="nl">balg</trd>
  <trdgrp lng="pl">
    <trd>miech</trd>,
    <trd>mieszek</trd>,
    <trd>harmonijka</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_h;
    <klr>(&c_k;&c_u;&c_z;&c_n;&c_je;&c_ch;&c_n;&c_y;&c_j;,
    &c_a;&c_k;&c_k;&c_o;&c_r;&c_d;&c_je;&c_o;&c_n;&c_a; &c_i;
    &c_t;.&c_p;.)</klr></trd>,
    <trd>&c_g;&c_a;&c_r;&c_m;&c_o;&c_sh;&c_k;&c_a;
    <klr>(&c_m;&c_je;&c_zh;&c_d;&c_u;
    &c_v;&c_a;&c_g;&c_o;&c_n;&c_a;&c_m;&c_i;)</klr></trd>,
    <trd>&c_o;&c_t;&c_k;&c_i;&c_d;&c_n;&c_o;&c_j;
    &c_v;&c_je;&c_r;&c_h; <klr>(&c_e;&c_k;&c_i;&c_p;&c_a;&c_zh;&c_a;,
    &c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;&c_i;&c_l;&c_ja;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>zriasen&yacute; mech <klr>(harmoniky, &zcaron;el. voz&ncaron;ov)</klr></trd>,
    <trd>zriasen&yacute; mech <klr>(harmoniky| &zcaron;el. voz&ncaron;ov)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_m;&c_ib;&c_h;</trd>,
    <trd>&c_g;&c_a;&c_r;&c_m;&c_o;&c_sh;&c_k;&c_a;&nbsp;(&c_m;&c_ib;&c_zh; &c_v;&c_a;&c_g;&c_o;&c_n;&c_a;&c_m;&c_i;)</trd>
  </trdgrp>
</drv>
</art>
