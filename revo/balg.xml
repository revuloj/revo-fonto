   <trd>harmonika<klr tip="ind"> (red&odblac;ny)</klr></trd>,
   <trd>l&eacute;gharmonika</trd>,
   <trd>kihuzat<klr tip="ind"> (f&eacute;nyk&eacute;pez&odblac;g&eacute;pen)</klr></trd>
</trdgrp>
<trd lng="nl">balg</trd>
