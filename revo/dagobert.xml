<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Dagobert</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="dagobert.0o">
  <kap><tld/>o</kap>
  <snc mrk="dagobert.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        li estas <klr>...</klr> vendisto de sanigaj herboj naskita en 
        Macondo kaj tie baptita <tld/>o
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>;
      </ekz>
      <ekz>
        en tiu loko la franka re&gcirc;o <tld/>o la 1-a dum la jaro 626 
        konstruigis abatejon kaj bazilikon honore al la sanktulo
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Denizo_de_Parizo">
          Denizo de Parizo</url>, 2010-02-15</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">Dagobert</trd>
  <trd lng="sk">Dagobert</trd>
</drv>
</art>

