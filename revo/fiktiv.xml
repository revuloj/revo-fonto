<kap>
  <ofc>1</ofc>
  <rad>fiktiv</rad>/a 
</kap>
<drv mrk="fiktiv.0a">
  <kap><tld/>a</kap>
  <snc mrk="fiktiv.0a.KOMUNE">
    <dif>
      <ref tip="dif" cel="fikci.0a">Fikcia</ref>:
      <ekz>
        ekzistas &ccirc;irka&ubreve; ducent dek verkoj de li,
        kiuj, kvankam la plejmulto estas <tld/>aj,
        ka&ubreve;zis tre viveman impreson
        <fnt>
          <aut>D. Defo, trad. A. Krafft</aut>,
          <vrk><url ref="&Gutenberg;files/11511/11511-0.txt">
          Robinsono Kruso</url></vrk>,
          <lok>1908</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj poemoj de reala Lojko <klr>[&#x2026;]</klr> permesas pli profunde eksenti la
        odoron de tiu epoko
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="&Monato;/2004/008392.html">Reveno de heroo</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_k;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>
  <trd lng="ca">fictici</trd>
  <trdgrp lng="cs">
    <trd>fiktivn&iacute;</trd>,
    <trd>imagin&aacute;rn&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
   <trd>fiktiv</trd>,
   <trd>ausgedacht</trd></trdgrp>
  <trd lng="en">fictitious</trd>
  <trd lng="es">ficticio</trd>
  <trdgrp lng="fr">
    <trd>fictif</trd>,
    <trd>fictionnel</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>fikt&iacute;v</trd>,
     <trd>kital&aacute;lt</trd>
   </trdgrp>
  <trd lng="nl">fictief</trd>
  <trdgrp lng="pl">
    <trd>zmy&sacute;lony</trd>,
    <trd>fikcyjny</trd>,
    <trd>urojony</trd>
  </trdgrp>
  <trd lng="pt">fict&iacute;cio</trd>
  <trd lng="ru">&c_f;&c_i;&c_k;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">neskuto&ccaron;n&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_f;&c_ib;&#x43A;&c_t;&c_i;&c_v;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43C;&#x43D;&c_i;&#x43C;&c_i;&c_j;</trd>,
    <trd>&c_u;&#x44F;&c_v;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_i;&c_g;&c_a;&c_d;&c_a;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="zh">&#x865A;&#x6784; <pr>x&#x16B;g&#xF2;u</pr></trd>
</drv>
