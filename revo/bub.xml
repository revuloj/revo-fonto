<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <ofc>*</ofc>
    <rad>bub</rad>/o
  </kap>

  <drv mrk="bub.0o">
    <kap>
      <ofc>*</ofc>
      <tld/>o
    </kap>
    <snc mrk="bub.0o.infano">
      <dif>
        Petola infano:
        <ekz>
          la aventuroj de la flandraj <tld/>oj Suske kaj Wiske estas
          konataj en &ccirc;iuj kontinentoj
          <fnt>
            <vrk>Cisko kaj Vinjo</vrk>
            <lok><url
            ref="http://eo.wikipedia.org/wiki/Cisko_kaj_Vinjo">Vikipedio</url></lok>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>Bube</trd>,
        <trd>Junge</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>imp</trd>,
        <trd>scamp</trd>,
        <trd>urchin</trd>
      </trdgrp>
      <trdgrp lng="es">
        <trd>alop&iacute;n</trd>,
        <trd>pilluelo</trd>,
        <trd>bribonzuelo</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>gamin</trd>,
        <trd>gamine</trd>,
        <trd>gosse</trd>,
        <trd>polisson</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>k&ouml;ly&ouml;k</trd>,
        <trd>csintalan <ind>k&ouml;ly&ouml;k</ind></trd>
      </trdgrp>
      <trd lng="io">bubo</trd>
      <trd lng="it">monello</trd>
      <trdgrp lng="nl">
        <trd>belhamel</trd>,
        <trd>bengel</trd>,
        <trd>blaag</trd>,
        <trd>boef</trd>,
        <trd>deugniet</trd>,
        <trd>doerak</trd>,
        <trd>dondersteen</trd>,
        <trd>donderstraal</trd>,
        <trd>kapoen</trd>,
        <trd>kwajongen</trd>,
        <trd>ondeugd</trd>,
        <trd>rakker</trd>,
        <trd>rekel</trd>,
        <trd>schavuit</trd>,
        <trd>schelm</trd>,
        <trd>schobbejak</trd>,
        <trd>snotaap</trd>,
        <trd>snotneus</trd>,
        <trd>vlegel</trd>,
        <trd>vlerk</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>brzd&aogonek;c</trd>,
        <trd>malec</trd>,
        <trd>maluch</trd>,
        <trd>smyk</trd>,
        <trd>b&aogonek;bel <klr>(pot.)</klr></trd>,
        <trd>f&aogonek;fel <klr>(pot.)</klr></trd>
      </trdgrp>
      <trd lng="pt">pirralho</trd>
      <trdgrp lng="ru">
        <trd>&c_m;&c_a;&c_l;&c_mol;&c_ch;&c_i;&c_sh;&c_k;&c_a;</trd>,
        <trd>&c_p;&c_r;&c_o;&c_k;&c_a;&c_z;&c_n;&c_i;&c_k;</trd>,
        <trd>&c_s;&c_o;&c_r;&c_v;&c_a;&c_n;&c_je;&c_c;</trd>
      </trdgrp>
    </snc>
    <snc mrk="bub.0o.karta">
      <fnt>PIV3</fnt>
      <ref tip="dif" cel="fant.0o.karta">fanto</ref>
      <trd lng="be">&c_v;&c_a;&c_l;&c_je;&c_t;</trd>
      <trd lng="br">lakez<klr tip="ind"> (kartenn c'hoari)</klr></trd>
      <trd lng="ca">jota<klr> (cartes)</klr></trd>
      <trdgrp lng="cs">
        <trd>fant</trd>,
        <trd>z&aacute;stava</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Bube <klr>[francaj bildoj]</klr></trd>,
        <trd>Unter <klr>[germanaj bildoj]</klr></trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>jack</trd>,
        <trd>knave</trd>
      </trdgrp>
      <trdgrp lng="es">
        <trd>sota</trd>,
        <trd>joto</trd>,
        <trd>jack</trd>
      </trdgrp>
      <trd lng="fr">valet<klr tip="ind"> (aux cartes)</klr></trd>
      <trdgrp lng="hu">
        <trd>filk&oacute;</trd>,
        <trd>bubi</trd>,
        <trd>als&oacute;<klr tip="ind"> (k&aacute;rty&aacute;ban)</klr></trd>
      </trdgrp>
      <trdgrp lng="it">
        <trd>fante</trd>,
        <trd>jack</trd>
      </trdgrp>
      <trd lng="nl">boer<klr tip="ind"> (in kaartspel)</klr></trd>
      <trdgrp lng="pl">
        <trd>walet</trd>,
        <trd>ch&lstroke;opak<klr> (pot.)</klr></trd>,
        <trd>ch&lstroke;opek<klr> (pot.)</klr></trd>,
        <trd>dupek<klr> (pot.)</klr></trd>
      </trdgrp>
      <trdgrp lng="pt">
        <trd>valete <klr tip="amb">(baralho)</klr></trd>,
        <trd>conde <klr tip="amb">(baralho)</klr></trd>
      </trdgrp>
      <trd lng="ru">&c_v;&c_a;&c_l;&c_je;&c_t;</trd>
      <trdgrp lng="sk">
        <trd>doln&iacute;k <klr>(karta)</klr></trd>,
        <trd>spodok</trd>
      </trdgrp>
    </snc>
  </drv>

  <drv mrk="bub.0ino">
    <kap><tld/>ino</kap>
    <snc>
      <dif>
        Inseksa <tld/>o<sncref ref="bub.0o.infano"/>.
      </dif>
    </snc>
    <trd lng="io">bubino</trd>
  </drv>

  <drv mrk="bub.0acxo">
    <kap><tld/>a&ccirc;o</kap>
    <snc>
      <dif>
        Malbonedukita <tld/>o<sncref ref="bub.0o.infano"/>.
      </dif>
      <trdgrp lng="de">
        <trd>Bengel</trd>,
        <trd>Schlingel</trd>,
        <trd>L&uuml;mmel</trd>
      </trdgrp>
      <trd lng="en">brat</trd>
      <trdgrp lng="fr">
        <trd>garnement</trd>,
        <trd>sale <ind>gosse</ind></trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>csib&eacute;sz</trd>,
        <trd>g&eacute;zeng&uacute;z</trd>
      </trdgrp>
      <trd lng="io">bubacho</trd>
      <trdgrp lng="pl">
        <trd>ga&lstroke;gan</trd>,
        <trd>&lstroke;obuz</trd>,
        <trd>&lstroke;obuziak</trd>
        <trd>urwis</trd>,
        <trd>szczeniak<klr> (obra&zacute;l.)</klr></trd>,
        <trd>andrus<klr> (przest.)</klr></trd>,
        <trd>ladaco<klr> (przest.)</klr></trd>,
        <trd>wisus<klr> (przest.)</klr></trd>
      </trdgrp>
      <trd lng="ro">c&acirc;rp&abreve;</trd>
      <trdgrp lng="ru">
        <trd>&c_sh;&c_a;&c_l;&c_o;&c_p;&c_a;&c_j;</trd>,
        <trd>&c_h;&c_u;&c_l;&c_i;&c_g;&c_a;&c_n;</trd>
      </trdgrp>
    </snc>
  </drv>
</art>

