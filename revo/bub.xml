<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bub.xml,v 1.27 2023/10/29 19:11:03 revo Exp $">
  <kap>
    <ofc>*</ofc>
    <rad>bub</rad>/o <fnt><bib>PV</bib></fnt>
  </kap>

  <drv mrk="bub.0o">
    <kap><ofc>*</ofc><tld/>o
    </kap>
    <snc mrk="bub.0o.infano">
      <dif>
        Petola knabo:
        <ekz>
          &ccirc;ia <tld/>o povas ankora&ubreve; mokadi nin kaj montradi nin per la
          fingroj
          <fnt>
            <aut>L. L. Zamenhof</aut>,
            <vrk>Esenco kaj Estonteco de la Ideo de Lingvo
            Internacia</vrk>,
            <lok>1907</lok>
          </fnt>;
        </ekz>
        <ekz>
          amaso da scivolemuloj kaj <tld/>oj haltis sur la trotuaro kaj
          rikanas
          <fnt>
            <bib>Metrop</bib>
          </fnt>;
        </ekz>
        <ekz>
          nun &scirc;i larmis amare kiel <tld/>o perdinta la amatan ludilon
          <fnt>
            <aut>C. Piron</aut>,
            <vrk>&Ccirc;u ni kunvenis vane?</vrk>,
            <lok>1982</lok>
          </fnt>;
        </ekz>
        <ekz>
          la aventuroj de la flandraj <tld/>oj <nom>Suske</nom> kaj
          <nom>Wiske</nom> estas konataj en &ccirc;iuj kontinentoj
          <fnt>
            <aut>P. Peeraerts</aut>,
            <vrk><url ref="http://esperanto-forum.org/index.php?topic=764.0">La
            patro de Cisko kaj Vinjo 100 jara!</url></vrk>,
            <lok>Esperanto-forumo, 2013-02-15</lok>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="be">
        <trd>&c_v;&c_ib;&c_s;&c_u;&c_s;</trd>,
        <trd>&c_g;&c_a;&c_r;&c_e;&c_z;&c_a;</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Bube</trd>,
        <trd>Junge</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>imp</trd>,
        <trd>scamp</trd>,
        <trd>urchin</trd>
      </trdgrp>
      <trdgrp lng="es">
        <trd>alop&iacute;n</trd>,
        <trd>pilluelo</trd>,
        <trd>bribonzuelo</trd>
      </trdgrp>
      <trd lng="fa">&f_p;&a_s;&a_r;&a_b;&f_ch;&a_h;</trd>
      <trdgrp lng="fr">
        <trd>gamin</trd>,
        <trd>gosse</trd>,
        <trd>polisson</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>k&ouml;ly&ouml;k</trd>,
        <trd>csintalan <ind>k&ouml;ly&ouml;k</ind></trd>
      </trdgrp>
      <trd lng="io">bubo</trd>
      <trd lng="it">monello</trd>
      <trdgrp lng="nl">
        <trd>deugniet</trd>,
        <trd>kwajongen</trd>,
        <trd>snotaap</trd>,
        <trd>snotneus</trd>,
        <trd>vlerk</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>brzd&aogonek;c</trd>,
        <trd>malec</trd>,
        <trd>maluch</trd>,
        <trd>smyk</trd>,
        <trd>b&aogonek;bel <klr>(pot.)</klr></trd>,
        <trd>f&aogonek;fel <klr>(pot.)</klr></trd>
      </trdgrp>
      <trd lng="pt">pirralho</trd>
      <trdgrp lng="ru">
        <trd>&c_m;&c_a;&c_l;&c_mol;&c_ch;&c_i;&c_sh;&c_k;&c_a;</trd>,
        <trd>&c_p;&c_r;&c_o;&c_k;&c_a;&c_z;&c_n;&c_i;&c_k;</trd>,
        <trd>&c_s;&c_o;&c_r;&c_v;&c_a;&c_n;&c_je;&c_c;</trd>
      </trdgrp>
      <trd lng="zh">&#x5D3D; <pr>z&#x1CE;i</pr></trd>
</snc>
    <snc mrk="bub.0o.karta">
      <fnt>PIV3</fnt>
      <ref tip="dif" cel="fant.0o.karta">fanto</ref>
      <trd lng="be">&c_v;&c_a;&c_l;&c_je;&c_t;</trd>
      <trd lng="br">lakez<klr tip="ind"> (kartenn c'hoari)</klr></trd>
      <trd lng="ca">jota<klr> (cartes)</klr></trd>
      <trdgrp lng="cs">
        <trd>fant</trd>,
        <trd>z&aacute;stava</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Bube <klr>[francaj bildoj]</klr></trd>,
        <trd>Unter <klr>[germanaj bildoj]</klr></trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>jack</trd>,
        <trd>knave</trd>
      </trdgrp>
      <trdgrp lng="es">
        <trd>sota</trd>,
        <trd>joto</trd>,
        <trd>jack</trd>
      </trdgrp>
      <trd lng="fa">&a_s;&a_r;&a_b;&a_A;&a_z;<klr tip="ind"> (&a_w;&a_r;&a_q; &a_b;&a_A;&a_z;&f_y;)</klr></trd>
      <trd lng="fr">valet<klr tip="ind"> (aux cartes)</klr></trd>
      <trd lng="fy">boer<klr tip="ind"> (yn kaartspul)</klr></trd>
      <trdgrp lng="hu">
        <trd>filk&oacute;</trd>,
        <trd>bubi</trd>,
        <trd>als&oacute;<klr tip="ind"> (k&aacute;rty&aacute;ban)</klr></trd>
      </trdgrp>
      <trdgrp lng="it">
        <trd>fante</trd>,
        <trd>jack</trd>
      </trdgrp>
      <trd lng="nl">boer<klr tip="ind"> (in kaartspel)</klr></trd>
      <trdgrp lng="pl">
        <trd>walet</trd>,
        <trd>ch&lstroke;opak<klr> (pot.)</klr></trd>,
        <trd>ch&lstroke;opek<klr> (pot.)</klr></trd>,
        <trd>dupek<klr> (pot.)</klr></trd>
      </trdgrp>
      <trdgrp lng="pt">
        <trd>valete <klr tip="amb">(baralho)</klr></trd>,
        <trd>conde <klr tip="amb">(baralho)</klr></trd>
      </trdgrp>
      <trd lng="ru">&c_v;&c_a;&c_l;&c_je;&c_t;</trd>
      <trdgrp lng="sk">
        <trd>doln&iacute;k <klr>(karta)</klr></trd>,
        <trd>spodok</trd>
      </trdgrp>
    </snc>
    <trd lng="he">&#x5E4;&#x5E8;&#x5D7;&#x5D7;</trd>
  <trdgrp lng="ja">
    <trd>&#x3044;&#x305F;&#x305A;&#x3089;&#x3063;&#x5B50;  <pr>&#x3044;&#x305F;&#x305A;&#x3089;&#x3063;&#x3053;</pr></trd>,
    <trd>&#x30B8;&#x30E3;&#x30C3;&#x30AF;  <pr>&#x3058;&#x3083;&#x3063;&#x304F;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_h;&#x43B;&#x43E;&#x43F;&c_ch;&c_a;&#x43A;</trd>,
    <trd>&c_h;&#x43B;&#x43E;&#x43F;&c_ch;&c_i;&c_s;&#x44C;&#x43A;&#x43E;</trd>,
    <trd>&#x43F;&c_a;&c_c;&c_a;&#x43D;</trd>,
    <trd>&#x43F;&c_u;&c_s;&c_t;&c_u;&#x43D;</trd>,
    <trd>&c_sh;&c_i;&c_b;&c_je;&#x43D;&c_i;&#x43A;</trd>
  </trdgrp>
</drv>

  <drv mrk="bub.0ino">
    <kap><tld/>ino</kap>
    <snc mrk="bub.0ino.HOM">
      <dif>
        Petola knabino:
        <ekz>
          &rightquot;vi abomeninda <tld/>ino!&#x201D; diris la patrino
          <fnt>
            <aut>H. C. Andersen, trad. d-ro L. L. Zamenhof</aut>,
            <vrk><url ref="&Steloj;fabeloj2/negxregxino.html">Fabeloj
            de Andersen &#x2010; Malgranda rabistinfano</url></vrk>
          </fnt>;
        </ekz>
        <ekz>
          ni estas nur du naivaj <tld/>inoj
          <fnt>
            <aut>C. Rossetti</aut>,
            <vrk>Kredu Min, Sinjorino!</vrk>,
            <lok>1950</lok>
          </fnt>.
        </ekz>
      </dif>
    </snc>
    <trd lng="be">&c_g;&c_a;&c_r;&c_e;&c_z;&c_mol;&c_n;&c_ib;&c_c;&c_a;</trd>
    <trd lng="fa">&a_d;&a_kh;&a_t;&a_r;&a_b;&f_ch;&a_h;</trd>
    <trd lng="fr">gamine</trd>
    <trd lng="io">bubino</trd>
    <trd lng="zh">&#x81ED;&#x5C0F;&#x5B50; <pr>ch&#xF2;uxi&#x1CE;ozi</pr></trd>
</drv>

  <drv mrk="bub.0acxo">
    <kap><tld/>a&ccirc;o</kap>
    <snc mrk="bub.0acxo.HOM">
      <dif>
        Malbonedukita <tld/>o<sncref ref="bub.0o.infano"/>:
        <ekz>
          kion vi faras tie &ccirc;i, buba&ccirc;oj?
          <fnt>
            <bib>InfanTorent2</bib>
          </fnt>;
        </ekz>
        <ekz>
          la dan&gcirc;ero de la hipopotamo ne malinstigis <tld/>a&ccirc;ojn &jcirc;eti
          &scirc;tonojn en la akvon por aperigi la beston
          <fnt>
            <aut>J.-L. Madzella</aut>,
            <vrk>&Gcirc;ua blekado</vrk>,
            <bib>Monato</bib>
            <lok>jaro 2013a</lok>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="be">
        <trd>&c_sh;&c_a;&c_l;&c_a;&c_p;&c_u;&c_t;</trd>,
        <trd>&c_h;&c_u;&c_l;&c_ib;&c_g;&c_a;&c_n;</trd>,
        <trd>&c_g;&c_a;&c_l;&c_g;&c_a;&c_n;</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Bengel</trd>,
        <trd>Schlingel</trd>,
        <trd>L&uuml;mmel</trd>
      </trdgrp>
      <trd lng="en">brat</trd>
      <trdgrp lng="fa">
        <trd>&a_b;&f_ch;&a_h;&a_hamza_sure; &a_sh;&f_y;&a_T;&a_A;&a_n;</trd>,
        <trd>&a_b;&f_ch;&a_h;&a_hamza_sure; &a_t;&a_kh;&a_s;</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>garnement</trd>,
        <trd>sale <ind>gosse</ind></trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>csib&eacute;sz</trd>,
        <trd>g&eacute;zeng&uacute;z</trd>
      </trdgrp>
      <trd lng="io">bubacho</trd>
      <trdgrp lng="pl">
        <trd>ga&lstroke;gan</trd>,
        <trd>&lstroke;obuz</trd>,
        <trd>&lstroke;obuziak</trd>
        <trd>urwis</trd>,
        <trd>szczeniak<klr> (obra&zacute;l.)</klr></trd>,
        <trd>andrus<klr> (przest.)</klr></trd>,
        <trd>ladaco<klr> (przest.)</klr></trd>,
        <trd>wisus<klr> (przest.)</klr></trd>
      </trdgrp>
      <trd lng="ro">c&acirc;rp&abreve;</trd>
      <trdgrp lng="ru">
        <trd>&c_sh;&c_a;&c_l;&c_o;&c_p;&c_a;&c_j;</trd>,
        <trd>&c_h;&c_u;&c_l;&c_i;&c_g;&c_a;&c_n;</trd>
      </trdgrp>
      <trd lng="zh">&#x9811;&#x7AE5; <pr>w&#xE1;nt&#xF3;ng</pr></trd>
</snc>
    <trd lng="he">&#x5E4;&#x5E8;&#x5D7;&#x5D7;</trd>
  <trdgrp lng="ja">
    <trd>&#x975E;&#x884C;&#x5C11;&#x5E74;  <pr>&#x3072;&#x3053;&#x3046;&#x3057;&#x3087;&#x3046;&#x306D;&#x3093;</pr></trd>,
    <trd>&#x304C;&#x304D;</trd>
  </trdgrp>
</drv>

<drv mrk="bub.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="bub.0ajxo.KOMUNE">
    <dif>
      Ago, precipe malica, &scirc;erca, de bubo:
      <ekz>
        vi ne bezonas nomi tion idiota <tld/>a&jcirc;o
        <fnt>
          <aut>Stellan Engholm</aut>,
          <vrk>Vivo vokas</vrk>,
          <lok>1946</lok>
        </fnt>;
      </ekz>
      <ekz>
        tute ordinara <tld/>a&jcirc;o &ndash; el kiu oni faris kolosan aferon
        <fnt>
          <aut>J. Pru&scirc;a, trad. A. Stanura</aut>
          <vrk><url ref="&DonH;Revuoj/nlr/nlr44/cigana.html">Cigana
          historio</url></vrk>
          <lok>la nica literatura revuo, 1959-03 &gcirc;is 04 (4/4), p.
          147a-151a</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ne faris <tld/>a&jcirc;ojn
        <fnt>
          <bib>Kiso</bib>,
          <lok>anta&ubreve;parolo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fa">&a_sh;&f_y;&a_T;&a_n;&a_t;</trd>
    <trd lng="fr">polissonnerie</trd>
  </snc>
  <trd lng="be">&c_s;&c_v;&c_a;&c_v;&c_o;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_a;</trd>
  <trd lng="he">&#x5DE;&#x5E2;&#x5E9;&#x5D4; &#x5E7;&#x5D5;&#x5E0;&#x5D3;&#x5E1;</trd>
  <trd lng="ja">&#x3044;&#x305F;&#x305A;&#x3089;</trd>
  <trdgrp lng="uk">
    <trd>&c_h;&#x43B;&#x43E;&#x43F;&#x2019;&#x44F;&c_c;&c_t;&c_v;&#x43E;</trd>,
    <trd>&c_b;&c_je;&c_sh;&#x43A;&c_je;&c_t;&#x43D;&c_i;&c_c;&c_t;&c_v;&#x43E;</trd>
  </trdgrp>
</drv>

</art>
