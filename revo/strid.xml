<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: strid.xml,v 1.9 2023/11/23 21:23:56 revo Exp $">
<kap>
  <rad>strid</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="strid.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="strid.0i.SON">
    <dif>
      <ref tip="super" cel="bru.0i.soni">Bruadi</ref> la&ubreve;te kaj
      akre, malagrable:
      <ekz>
        malanta&ubreve; &scirc;ia dorso en la dormo&ccirc;ambro ek<tld/>is la telefono
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 20a, p. 202a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>je&#x10D;et</trd>,
    <trd>v&rcaron;e&scaron;t&#x11B;t</trd>
  </trdgrp>
  <trd lng="fr">crever les tympans</trd>
  <trdgrp lng="hu">
    <trd>siv&iacute;t</trd>,
    <trd>s&uuml;v&iacute;t</trd>,
    <trd>sip&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">huilen</trd>
  <trdgrp lng="pl">
    <trd>przenika&cacute;</trd>,
    <trd>d&zacute;wi&eogonek;cze&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_z;&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol; &c_r;&c_je;&c_z;&c_k;&c_i;&c_je; &c_z;&c_v;&c_u;&c_k;&c_i;</trd>
  <trdgrp lng="sk">
    <trd>hvi&#x17E;&#x10F;a&tcaron;</trd>,
    <trd>ja&#x10D;a&tcaron;</trd>,
    <trd>vre&scaron;&tcaron;a&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_ib;&c_z;&c_a;&c_t;&c_i; &c_s;&#x43B;&c_u;&c_h;</trd>,
    <trd>&#x43F;&c_i;&c_shch;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&#x43A;&c_r;&c_i;&#x43F;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_s;&#x43A;&c_r;&c_je;&#x43A;&#x43E;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &c_g;&#x43E;&c_s;&c_t;&c_r;&c_i;&c_j;</trd>,
    <trd>&c_r;&c_ib;&c_z;&#x43A;&c_i;&c_j; &c_z;&c_v;&c_u;&#x43A;/&c_g;&#x43E;&#x43B;&#x43E;&c_s;</trd>
  </trdgrp>
</drv>

<drv mrk="strid.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="strid.0a.SON">
    <dif>
      Akra kaj la&ubreve;ta:
      <ekz>
        &scirc;i jelpe kaj <tld/>e ploras
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 82a, volumo 3a, p.
        25a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">strident</trd>
  <trdgrp lng="hu">
    <trd>siv&iacute;t&oacute;</trd>,
    <trd>f&uuml;lsiket&iacute;t&odblac;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>oorverscheurend</trd>,
    <trd>snijdend</trd>,
    <trd>schel</trd>,
    <trd>door <ind>merg</ind> en been gaand</trd>
  </trdgrp>
  <trd lng="pl">ostry  <klr>(d&zacute;wi&eogonek;k)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_z;&c_k;&c_i;&c_j;<klr tip="amb"> (&c_o; &c_z;&c_v;&c_u;&c_k;&c_je;)</klr></trd>,
    <trd>&c_p;&c_r;&c_o;&c_n;&c_z;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_i;&c_s;&#x43A;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_r;&#x43E;&#x43D;&c_i;&c_z;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_s;&#x43A;&c_r;&c_i;&#x43F;&c_u;&c_ch;&c_i;&c_j;</trd>,
    <trd>&c_s;&#x43A;&c_r;&c_i;&#x43F;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_r;&c_ib;&c_z;&#x43A;&c_i;&c_j;</trd>,
    <trd>&c_g;&#x43E;&c_s;&c_t;&c_r;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>

