<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hetajr</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="hetajr.0o">
  <kap><tld/>o</kap>
  <snc mrk="hetajr.0o.MOR">
    <dif>
      Alte kulturita kaj altsocieta
      <ref tip="super" cel="prosti.0itino.AMOR">prostituitino</ref>:
      <ekz>
        <ctl>Anekdota Biografio de Li Shishi</ctl>
        estas amrakonto inter &ccirc;arma <tld/>o Li Shishi
        kaj imperiestro
        <fnt>
          <aut>Shi Chengtai</aut>,
          <vrk><url
          ref="http://www.espero.com.cn/old/epch/05e/05e-09r/09-05.htm">
          Gemoj en la verda mondo</url></vrk>,
          <lok>[2009?]</lok>
        </fnt>.        
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_t;&c_e;&c_r;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>hetera</trd>,
    <trd>hetaira</trd>
  </trdgrp>
  <trd lng="de">Het&auml;re</trd>
  <trd lng="el">&epsilon;&tau;&alfa;&jota_ton;&rho;&alfa;</trd>
  <trdgrp lng="en">
    <trd>hetaera</trd>,
    <trd>hetaira</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cocotte</trd>,
    <trd>courtisane<klr> (cocotte)</klr></trd>,
    <trd>heta&iuml;re</trd>
  </trdgrp>
  <trd lng="hu">het&eacute;ra</trd>
  <trd lng="it">etera</trd>
  <trd lng="nl">hetaera</trd>
  <trd lng="pl">hetera</trd>
  <trd lng="pt">hetera</trd>
  <trd lng="ru">&c_g;&c_je;&c_t;&c_je;&c_r;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>hetéra</trd>,
    <trd>kurtizána</trd>,
    <trd>milenka</trd>,
    <trd>neviestka <klr>(vo vyšších kruhoch)</klr></trd>
  </trdgrp>
</drv>
</art>
