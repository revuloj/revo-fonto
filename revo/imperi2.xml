<art mrk="$Id: imperi2.xml,v 1.22 2023/11/19 10:22:12 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>imperialism</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="imperi2.0o">
  <kap><tld/>o</kap>
  <snc mrk="imperi2.0o.imperiismo">
    <dif>
      <ref tip="dif" cel="imperi.0ismo">Imperiismo</ref>:
      <ekz>
         la nokta masakrado, aran&gcirc;ita de la japana <tld/>o,
         <klr>[&#x2026;]</klr> restos neforvi&scirc;ebla memoro
         <fnt><aut>J. Baghy</aut>, <vrk>Sur Sanga Tero</vrk>, 
         <lok>1933</lok></fnt>;      
      </ekz>
      <ekz>
        la avida mamonismo, la impertinenta <tld/>o, la aroganta naciismo kaj la cinika
        makiavelismo &ndash; tiuj estas la mensogemaj fratoj
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2005/008677.php">Frato Pa&ubreve;lo: pacamanto</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        la konferenco celis liberigi aziajn kaj afrikajn naciojn de <tld/>o kaj
        koloniismo, kaj krei ekonomian kaj kulturan kunlaboron inter la koncernaj
        landoj
        <fnt>
          <bib>Monato</bib>
          <aut>Syauqi Ahmad Zulfauzi Stya Lacksana</aut>
          <vrk><url ref="&Monato;/2015/011595.php">Aktivas la bonaj amikoj</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_m;&c_p;&c_e;&c_r;&c_y;&c_ja;&c_l;&c_ib;&c_z;&c_m;</trd>
  <trd lng="bg">&c_i;&c_m;&c_p;&c_je;&c_r;&c_i;&c_a;&c_l;&c_i;&c_z;&c_malmol;&c_m;</trd>
  <trd lng="cs">imperializmus</trd>
  <trdgrp lng="de">
     <trd>Imperialismus</trd>,
     <trd>Kriegstreiberei</trd>
  </trdgrp>
  <trd lng="es">imperialismo</trd>
  <trd lng="fr">imperialisme</trd>
  <trd lng="hu">imperializmus</trd>
  <trd lng="nl">imperialisme</trd>
  <trd lng="pt">imperialismo</trd>
  <trd lng="ru">&c_i;&c_m;&c_p;&c_je;&c_r;&c_i;&c_a;&c_l;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">imperializmus</trd>
  <trd lng="uk">&#x456;&#x43C;&#x43F;&#x435;&#x440;&#x456;&#x430;&#x43B;&#x456;&#x437;&#x43C;</trd>
  <trd lng="zh">&#x5E1D;&#x56FD;&#x4E3B;&#x4E49; <pr>d&#xEC;gu&#xF3;zh&#x1D4;y&#xEC;</pr></trd>
