<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: epikar.xml,v 1.21 2025/04/26 18:34:07 revo Exp $">
<kap>
<rad>epikarp</rad>/o
</kap>
<drv mrk="epikar.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Ekstera parto de la <ref tip="malprt" cel="perika1.0o">perikarpo</ref>:
      <ekz>
        la kokoso <klr>[&#8230;]</klr> povas drivi de suda Hindio &gcirc;is suda Ameriko restante
        &gcirc;ermkapabla, la fibra mezokarpo servas kiel skusorbilo kaj la <tld/>o kiel
        barilo por jonoj (salakvo)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=522711">Hidro&hcirc;orio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="endoka1.0o">endokarpo</ref>,
      <ref cel="mezoka.0o">mezokarpo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_d;&c_p;&c_l;&c_o;&c_d;&c_n;&c_ib;&c_k;</trd>
  <trd lng="ca">epicarpi</trd>
  <trdgrp lng="cs">
    <trd>slupka</trd>,
    <trd>vn&ecaron;j&scaron;&iacute; oplod&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Epikarpo</trd>,
     <trd>&auml;u&szlig;erste Schicht der Fruchtschale</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>epicarp</trd>,
    <trd>exocarp</trd>
  </trdgrp>
  <trd lng="fr">&eacute;picarpe</trd>
  <trdgrp lng="hu">
    <trd>k&uuml;ls&odblac; <ind>term&eacute;sfal</ind></trd>,
    <trd>epikarpium</trd>
  </trdgrp>
  <trd lng="ja">&#22806;&#26524;&#30382;  <pr>&#12364;&#12356;&#12363;&#12402;</pr></trd>
  <trdgrp lng="pl">
    <trd>sk&oacute;rka<klr> owocu</klr></trd>,
    <trd>obowocnia</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>epicarpo</trd>,
    <trd>exocarpo</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_d;&c_p;&c_l;&c_o;&c_d;&c_n;&c_i;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>vonkaj&scaron;ie oplodie</trd>,
    <trd>&scaron;upka</trd>
  </trdgrp>
  <trd lng="uk">&c_n;&c_a;&c_d;&c_p;&c_l;&c_ib;&c_d;&c_n;&c_i;&c_k;</trd>
</drv>
</art>

