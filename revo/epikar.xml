<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>epikarp</rad>/o
</kap>
<drv mrk="epikar.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Ekstera parto de la <ref tip="malprt" 
      cel="perika1.0o">perikarpo</ref>.
    </dif>
    <refgrp tip="vid">
      <ref cel="endoka1.0o">endokarpo</ref>,
      <ref cel="mezoka.0o">mezokarpo</ref>
    </refgrp>
  </snc>
  <trd
  lng="be">&c_n;&c_a;&c_d;&c_p;&c_l;&c_o;&c_d;&c_n;&c_ib;&c_k;</trd>
  <trd lng="ca">epicarpi</trd>
  <trdgrp lng="cs">
    <trd>slupka</trd>,
    <trd>vnější oplodí</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Epikarpo</trd>,
     <trd>&auml;u&szlig;erste Schicht der Fruchtschale</trd>
  </trdgrp>
  <trd lng="fr">&eacute;picarpe</trd>
  <trdgrp lng="hu">
    <trd>k&uuml;ls&odblac; <ind>term&eacute;sfal</ind></trd>,
    <trd>epikarpium</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>sk&oacute;rka<klr> owocu</klr></trd>,
    <trd>obowocnia</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>epicarpo</trd>,
    <trd>exocarpo</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_d;&c_p;&c_l;&c_o;&c_d;&c_n;&c_i;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>vonkajšie oplodie</trd>,
    <trd>šupka</trd>
  </trdgrp>
</drv>
</art>

