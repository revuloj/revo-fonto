<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: inicial.xml,v 1.3 2022/08/30 09:32:37 revo Exp $">
    <kap>
      <rad>inicial</rad>/o
    </kap>

<drv mrk="inicial.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Komenca <ref tip="super" cel="liter.0o.propra">litero</ref> de nomo, pa&gcirc;o, 
      &ccirc;apitro ktp. foje tie aparte ornamita kaj kolorigita:
      <ekz>
        MMM (germanaj inicialoj de Mai-Markt Mannheim, Manhajma Maja Foiro)
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="https://www.monato.net/2004/008360.html">Fiere foire</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li aldonis la <tld/>ojn de la anta&ubreve;nomoj (J kaj J)
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="https://www.monato.net/2010/010142.php">JJB-breco</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la nomo ABBA estis formita el la <tld/>oj de la anta&ubreve;nomoj de la membroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=337">ABBA</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        bildoj &scirc;ajne ne ekzistis, sed mirinde belaj grandaj <tld/>oj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Sen&ccirc;esa Rakonto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_ib;&c_n;&c_ib;&c_c;&c_y;&c_ja;&c_l; <klr>(&c_p;&c_a;&c_ch;&c_a;&c_t;&c_k;&c_o;&c_v;&c_a;&c_ja; &c_l;&c_ib;&c_t;&c_a;&c_r;&c_a;)</klr></trd>,
      <trd>&c_b;&c_u;&c_k;&c_v;&c_ib;&c_c;&c_a;</trd>
    </trdgrp>
    <trd lng="de">Initiale</trd>
  </snc>
  <trd lng="uk">&#x43F;&#x43E;&c_ch;&c_a;&c_t;&#x43A;&#x43E;&c_v;&c_a; &c_b;&c_u;&#x43A;&c_v;&c_a; </trd>
</drv>

  </art>

