<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>martes</rad>/o
</kap>
<drv mrk="martes.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="mustel1.0oj">musteledoj</ref>
      (<trd lng="la">Martes</trd>), al kiu apartenas
      <refgrp tip="sub">
        <ref cel="foin.0o">foino</ref>,
        <ref cel="martes.0o.2">marteso 2</ref>,
        <ref cel="zibel.0o">zibelo</ref>.
      </refgrp>
    </dif>
  </snc>
  <snc mrk="martes.0o.2">
    <dif>
      Specio el tiu genro (<trd lng="la">Martes martes</trd>),
      kun flava brustmakulo.
    </dif>
    <trd lng="de">Baummarder</trd>
    <trd lng="nl">boommarter</trd>
  </snc>
  <trd lng="ru">&c_k;&c_u;&c_n;&c_i;&c_c;&c_a;</trd>
</drv>
</art>
