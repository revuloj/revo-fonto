<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>martes</rad>/o
</kap>
<drv mrk="martes.0o">
  <kap><tld/>o</kap>
  <snc mrk="martes.0o.ZOO_genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="mustel1.0oj">musteledoj</ref>
      (<trd lng="la">Martes</trd>), al kiu apartenas
      <refgrp tip="sub">
        <ref cel="foin.0o">foino</ref>,
        <ref cel="martes.0o.ZOO_specio">marteso <sncref/></ref>,
        <ref cel="zibel.0o">zibelo</ref>.
      </refgrp>
    </dif>
  </snc>
  <snc mrk="martes.0o.ZOO_specio">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Specio de <ref tip="super" cel="martes.0o.ZOO_genro">marteso
      <sncref/></ref> (<trd lng="la">Martes martes</trd>) kun flava
      brustmakulo, &ccirc;asanta birdojn, sciurojn kaj aliajn en arboj.
    </dif>
    <trd lng="ca">mart</trd>
    <trd lng="cs">kuna lesn&iacute;</trd>
    <trd lng="de">Baummarder</trd>
    <trdgrp lng="fr">
      <trd>marte</trd>,
      <trd>martre</trd>
    </trdgrp>
    <trd lng="it">martora</trd>
    <trd lng="nl">boommarter</trd>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="hu">nyuszt</trd>
  <trd lng="pt">marta <klr>(zool.)</klr></trd>
  <trd lng="ru">&c_k;&c_u;&c_n;&c_i;&c_c;&c_a;</trd>
</drv>
</art>
