<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>hierar&hcirc;i</rad>/o
</kap>
<drv mrk="hierar.0o">
  <kap><tld/>o</kap>
  <ref tip="dif" cel="hierarki.0o">hierarkio</ref>
  <ekz>
    militista, eklezia, katolika <tld/>o;
  </ekz>
  <ekz>
    malofta estas la landoj, kie la militistaj oficoj estas
    metitaj en la <tld/>on pli malsupre ol la civilaj.
  </ekz>
  <trdgrp lng="be">
    <trd>&c_ib;&c_je;&c_r;&c_a;&c_r;&c_h;&c_ib;&c_ja;</trd>,
    <trd>&c_g;&c_ib;&c_je;&c_r;&c_a;&c_r;&c_h;&c_ib;&c_ja;</trd>
  </trdgrp>
  <trd lng="de">Hierarchie</trd>
  <trd lng="el">&jota;&epsilon;&rho;&alfa;&rho;&chi;&jota_ton;&alfa;</trd>
  <trd lng="en">hierarchy</trd>
  <trd lng="es">jerarqu&iacute;a</trd>
  <trd lng="fr">hi&eacute;rarchie</trd>
  <trdgrp lng="hu">
    <trd>rangsor</trd>,
    <trd>hierarchia</trd>
  </trdgrp>
  <trd lng="it">gerarchia</trd>
  <trd lng="nl">hierarchie</trd>
  <trd lng="pl">hierarchia</trd>
  <trd lng="pt">hierarquia</trd>
  <trd lng="ru">&c_i;&c_je;&c_r;&c_a;&c_r;&c_h;&c_i;&c_ja;</trd>
</drv>
</art>
