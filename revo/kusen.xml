<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kusen</rad>/o
</kap>
<drv mrk="kusen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Sako el &scirc;tofo a&ubreve; ledo, remburita per mola&jcirc;oj (plumoj, haregoj)
      por ke oni povu sin komfortigi sur &gcirc;i:
      <ekz>
        lit<tld/>o, pied<tld/>o;
      </ekz>
      <ekz>
        mola lanuga <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        konscienco senmakula estas <tld/>o plej mola
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>cushion</trd>,
    <trd>pillow</trd>
  </trdgrp>
  <trd lng="de">Kissen</trd>
  <trd lng="es">coj&iacute;n</trd>
  <trd lng="fr">coussin</trd>
  <trd lng="fi">tyyny</trd>
  <trd lng="it">cuscino</trd>
  <trd lng="nl">kussen</trd>
  <trd lng="pl">poduszka</trd>
  <trd lng="pt">almofada</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trd lng="sv">kudde</trd>
</drv>

<drv mrk="kusen.kap0o">
  <kap>kap<tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="kusen.0o"><tld lit="K"/>o</ref> destinita por
      apogi la kapon, ekzemple por dormo:
      <ekz>
        subite, apud la kap<tld/>o de la &scirc;ajne morta korpo aperis bela
        junulo
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 69a, p.
        784a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>oreiller</trd>,
    <trd>polochon</trd>,
    <trd>traversin</trd>
  </trdgrp>
</drv>

<drv mrk="kusen.0tego">
  <kap><tld/>tego, <tld/>tegilo, <tld/>sako</kap>
  <snc>
    <dif>
      &Scirc;an&gcirc;ebla &scirc;tofa sako, en kiun oni metas kusenon.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>pillow case</trd>,
    <trd>cushion cover</trd>
  </trdgrp>
  <trd lng="de">Kissenbezug</trd>
  <trdgrp lng="es">
    <trd>funda de <ind>coj&iacute;n</ind></trd>,
    <trd>funda de <ind>almohada</ind></trd>
  </trdgrp>
  <trd lng="fr">taie</trd>
  <trd lng="fi">tyynyliina</trd>
  <trd lng="it">federa</trd>
  <trdgrp lng="nl">
    <trd>sloop</trd>,
    <trd>kussensloop</trd>
  </trdgrp>
  <trd lng="pl">poszewka</trd>
  <trd lng="pt">fronha</trd>
  <trd lng="ru">&c_n;&c_a;&c_v;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a;</trd>
  <trdgrp lng="sv">
    <trd>&ouml;rngott</trd>,
    <trd>dynvar</trd>
  </trdgrp>
</drv>
</art>
