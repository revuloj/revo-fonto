<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kusen.xml,v 1.30 2023/09/01 15:46:46 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kusen</rad>/o
</kap>

<drv mrk="kusen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Sako el &scirc;tofo a&ubreve; ledo, remburita per mola&jcirc;oj (plumoj, haregoj)
      por ke oni povu sin komfortigi sur &gcirc;i:
      <ekz>
        lit<tld/>o, pied<tld/>o;
      </ekz>
      <ekz>
        mola lanuga <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        konscienco senmakula estas <tld/>o plej mola
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trd lng="bg">&c_v;&c_malmol;&c_z;&c_g;&c_l;&c_a;&c_v;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="bo">&#xF66;&#xF94;&#xF66;&#xF0B;&#xF58;&#xF42;&#xF7C;&#xF0B;</trd>
  <trd lng="br">torchenn</trd>
  <trd lng="cs">pol&scaron;t&aacute;&rcaron;</trd>
  <trd lng="de">Kissen</trd>
  <trdgrp lng="en">
    <trd>cushion</trd>,
    <trd>throw pillow</trd>
  </trdgrp>
  <trd lng="es">coj&iacute;n</trd>
  <trd lng="fi">tyyny</trd>
  <trd lng="fr">coussin</trd>
  <trdgrp lng="hu">
    <trd>p&aacute;rna</trd>,
    <trd>v&aacute;nkos</trd>
  </trdgrp>
  <trd lng="id">bantal</trd>
  <trd lng="it">cuscino</trd>
  <trdgrp lng="ja">
    <trd>&#x30AF;&#x30C3;&#x30B7;&#x30E7;&#x30F3;</trd>,
    <trd>&#x5EA7;&#x5E03;&#x56E3;  [<ind>&#x3056;&#x3076;&#x3068;&#x3093;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">kussen</trd>
  <trd lng="pl">poduszka</trd>
  <trd lng="pt">almofada</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>podhlavnica</trd>,
    <trd>podu&scaron;ka</trd>,
    <trd>vank&uacute;&scaron;</trd>
  </trdgrp>
  <trd lng="sv">kudde</trd>
  <trd lng="uk">&#x43F;&#x43E;&#x434;&#x443;&#x448;&#x43A;&#x430;</trd>
</drv>

<drv mrk="kusen.0ego">
  <kap><ofc>*</ofc><tld/>ego <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="&AdE;/fundamento/universala_vortaro.html#av-5078">kusen'eg'</url></lok></fnt></kap>
  <snc>
    <dif>
    Longa sako el tolo, plenigita de plumoj, sur kiu oni povas ku&scirc;i.
    </dif>
  </snc>
    <trd lng="en">bolster</trd>
    <trdgrp lng="fr">
    <trd>couette</trd>,
    <trd>lit de plumes</trd>
  </trdgrp>
  <trd lng="ja">&#x5E03;&#x56E3;  [<ind>&#x3075;&#x3068;&#x3093;</ind>]</trd>
  <trd lng="uk">&#x43F;&#x435;&#x440;&#x438;&#x43D;&#x430;</trd>
</drv>

<drv mrk="kusen.kap0o">
  <kap>kap<tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="kusen.0o"><tld lit="K"/>o</ref> destinita
      por apogi la kapon, ekzemple por dormo:
      <ekz>
        subite, apud la kap<tld/>o de la &scirc;ajne morta korpo aperis bela
        junulo
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 69a, p.
        784a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_u;&c_sh;&c_k;&c_a; <klr>(&c_p;&c_a;&c_d; &c_g;&c_a;&c_l;&c_a;&c_v;&c_u;)</klr></trd>
  <trd lng="bg">&c_v;&c_malmol;&c_z;&c_g;&c_l;&c_a;&c_v;&c_n;&c_i;&c_c;&c_a;</trd>
  <trdgrp lng="br">
    <trd>goubenner</trd>,
    <trd>treuzplueg</trd>
  </trdgrp>
  <trd lng="cs">pol&scaron;t&aacute;&rcaron;</trd>
  <trd lng="de">Kopfkissen</trd>
  <trd lng="en">pillow</trd>
  <trd lng="es">almohada</trd>
  <trdgrp lng="fr">
    <trd>oreiller</trd>,
    <trd>polochon</trd>,
    <trd>traversin</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fejp&aacute;rna</trd>,
    <trd>fejv&aacute;nkos</trd>
  </trdgrp>
  <trd lng="id"><ind>bantal</ind> kepala</trd>
  <trd lng="ja">&#x6795;  [<ind>&#x307E;&#x304F;&#x3089;</ind>]</trd>
  <trd lng="nl">hoofdkussen</trd>
  <trd lng="pt">travesseiro</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_g;&c_o;&c_l;&c_o;&c_v;&c_mol;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_u;&c_sh;&c_k;&c_a;<klr tip="amb"> (&c_p;&c_o;&c_d; &c_g;&c_o;&c_l;&c_o;&c_v;&c_u;)</klr></trd>
  </trdgrp>
  <trd lng="sk">vank&uacute;&scaron;</trd>
  <trd lng="uk">&#x43F;&#x43E;&#x434;&#x443;&#x448;&#x43A;&#x430; &#x43F;&#x456;&#x434; &#x433;&#x43E;&#x43B;&#x43E;&#x432;&#x443;</trd>
</drv>

<drv mrk="kusen.0tego">
  <kap><tld/>tego, <var><kap><tld/>tegilo</kap></var>,
  <var><kap><tld/>sako</kap></var></kap>
  <snc>
    <dif>
      &Scirc;an&gcirc;ebla &scirc;tofa sako, en kiun oni metas kusenon.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_v;&c_a;&c_l;&c_a;&c_ch;&c_k;&c_a;</trd>
  <trd lng="bg">&c_k;&c_a;&c_l;&c_malmol;&c_f; &c_n;&c_a; &c_v;&c_malmol;&c_z;&c_g;&c_l;&c_a;&c_v;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="br">toagenn</trd>
  <trd lng="cs">povlak na pol&scaron;t&aacute;&rcaron;</trd>
  <trd lng="de">Kissenbezug</trd>
  <trdgrp lng="en">
    <trd>pillow case</trd>,
    <trd>cushion cover</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>funda de <ind>coj&iacute;n</ind></trd>,
    <trd>funda de <ind>almohada</ind></trd>
  </trdgrp>
  <trd lng="fi">tyynyliina</trd>
  <trd lng="fr">taie</trd>
  <trd lng="hu">p&aacute;rnahuzat</trd>
  <trd lng="id"><ind>sarung</ind> bantal</trd>
  <trd lng="it">federa</trd>
  <trdgrp lng="ja">
    <trd>&#x30AF;&#x30C3;&#x30B7;&#x30E7;&#x30F3;&#x30AB;&#x30D0;&#x30FC;</trd>,
    <trd>&#x5EA7;&#x5E03;&#x56E3;&#x30AB;&#x30D0;&#x30FC;  [<ind>&#x3056;&#x3076;&#x3068;&#x3093;&#x304B;&#x3070;&#x30FC;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>sloop</trd>,
    <trd>kussensloop</trd>
  </trdgrp>
  <trd lng="pl">poszewka</trd>
  <trd lng="pt">fronha</trd>
  <trd lng="ru">&c_n;&c_a;&c_v;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a;</trd>
  <trd lng="sk">oblie&ccaron;ka na vank&uacute;&scaron;</trd>
  <trdgrp lng="sv">
    <trd>&ouml;rngott</trd>,
    <trd>dynvar</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&#x430;&#x432;&#x43E;&#x43B;&#x43E;&#x447;&#x43A;&#x430;</trd>,
    <trd>&#x43F;&#x43E;&#x448;&#x438;&#x432;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>
</art>
