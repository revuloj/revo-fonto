<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kusen.xml,v 1.33 2023/12/06 14:28:18 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kusen</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="kusen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Sako el &scirc;tofo a&ubreve; ledo, remburita per mola&jcirc;oj (plumoj, haregoj)
      por ke oni povu sin komfortigi sur &gcirc;i:
      <ekz>
        la ju&gcirc;istoj sidis en siaj apogse&gcirc;oj kaj havis malanta&ubreve; la kapo <tld/>ojn el
        lanugo
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i e&ccirc; ricevis la permeson dormi sur velura <tld/>o anta&ubreve; lia pordo
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la gasto sin etendis sur la benko kaj sub la kapon metis la sakon kiel <tld/>on
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tablo, la orazeno kaj bastono el sako</lok>
        </fnt>;
      </ekz>
      <ekz>
        la viro ka&scirc;is la viza&gcirc;on en la lit<tld/>o <klr>(vd <ref cel="kusen.kap0o">kapkuseno</ref>)</klr>
        kaj malla&ubreve;te &gcirc;emis
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> e&ccirc; donis al &scirc;i malgrandan <ind>sid<tld/>o</ind>n
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
        <trd lng="de">Sitzkissen</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        konscienco senmakula estas <tld/>o plej mola
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="kusen.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Io mola, pufa, simila al <tld/>o:
      <ekz>
        li ekglitis sur musko<tld/>o
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Pramaljuna Morlino</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trd lng="bg">&c_v;&c_malmol;&c_z;&c_g;&c_l;&c_a;&c_v;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="bo">&t_sa;&t_ngata;&t_sa;&t_tsheg;&t_ma;&t_ga;&t_naro;&t_tsheg;</trd>
  <trd lng="br">torchenn</trd>
  <trd lng="cs">pol&scaron;t&aacute;&rcaron;</trd>
  <trd lng="de">Kissen</trd>
  <trdgrp lng="en">
    <trd>cushion</trd>,
    <trd>throw pillow</trd>
  </trdgrp>
  <trd lng="es">coj&iacute;n</trd>
  <trd lng="fi">tyyny</trd>
  <trd lng="fr">coussin</trd>
  <trdgrp lng="hu">
    <trd>p&aacute;rna</trd>,
    <trd>v&aacute;nkos</trd>
  </trdgrp>
  <trd lng="id">bantal</trd>
  <trd lng="it">cuscino</trd>
  <trdgrp lng="ja">
    <trd>&#12463;&#12483;&#12471;&#12519;&#12531;</trd>,
    <trd>&#24231;&#24067;&#22243;  <pr>&#12374;&#12406;&#12392;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">kussen</trd>
  <trd lng="pl">poduszka</trd>
  <trd lng="pt">almofada</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>podhlavnica</trd>,
    <trd>podu&scaron;ka</trd>,
    <trd>vank&uacute;&scaron;</trd>
  </trdgrp>
  <trd lng="sv">kudde</trd>
  <trd lng="uk">&c_p;&c_o;&c_d;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#33860; <pr>zu&ograve;</pr></trd>,
    <trd>&#26517; <pr>zh&ecaron;n</pr></trd>,
    <trd>&#26517;&#22836; <pr>zh&ecaron;ntou</pr></trd>,
    <trd>&#26517;&#38957; <pr>zh&ecaron;ntou</pr></trd>,
    <trd>&#24231;&#22443; <pr>zu&ograve;di&agrave;n</pr></trd>,
    <trd>&#24231;&#22666; <pr>zu&ograve;di&agrave;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kusen.0ego">
  <kap><ofc>*</ofc><tld/>ego <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="&AdE;/fundamento/universala_vortaro.html#av-5078">kusen'eg'</url></lok></fnt></kap>
  <snc>
    <dif>
      Longa sako el tolo, plenigita de plumoj, sur kiu oni povas ku&scirc;i:
      <ekz>
        kie ili prenis tiajn matracojn kaj <tld/>egojn? mi e&ccirc; ek&scirc;vitis
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        mankas la hejma <tld/>ego (kial tiaj ne ekzistas ekster Nordio?), sed la sofo
        plene ta&ubreve;gas kiel lito
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 3 (53)</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="matrac.0o.lita">matraco</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Polster</trd>,
    <trd>Federbett</trd>
  </trdgrp>
  <trd lng="en">bolster</trd>
  <trdgrp lng="fr">
    <trd>couette</trd>,
    <trd>lit de plumes</trd>
  </trdgrp>
  <trd lng="ja">&#24067;&#22243;  <pr>&#12405;&#12392;&#12435;</pr></trd>
  <trd lng="uk">&c_p;&c_je;&c_r;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#25265;&#26517; <pr>b&agrave;ozh&ecaron;n</pr></trd>,
    <trd>&#34987;&#23376; <pr>b&egrave;izi</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kusen.kap0o">
  <kap>kap<tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="kusen.0o"><tld lit="K"/>o</ref> destinita
      por apogi la kapon, ekzemple por dormo:
      <ekz>
        la konsilistoj sidis en siaj se&gcirc;oj kun la molaj kap<tld/>oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        subite, apud la kap<tld/>o de la &scirc;ajne morta korpo aperis bela
        junulo
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 69a, p.
        784a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_u;&c_sh;&c_k;&c_a; <klr>(&c_p;&c_a;&c_d; &c_g;&c_a;&c_l;&c_a;&c_v;&c_u;)</klr></trd>
  <trd lng="bg">&c_v;&c_malmol;&c_z;&c_g;&c_l;&c_a;&c_v;&c_n;&c_i;&c_c;&c_a;</trd>
  <trdgrp lng="br">
    <trd>goubenner</trd>,
    <trd>treuzplueg</trd>
  </trdgrp>
  <trd lng="cs">pol&scaron;t&aacute;&rcaron;</trd>
  <trd lng="de">Kopfkissen</trd>
  <trd lng="en">pillow</trd>
  <trd lng="es">almohada</trd>
  <trdgrp lng="fr">
    <trd>oreiller</trd>,
    <trd>polochon</trd>,
    <trd>traversin</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fejp&aacute;rna</trd>,
    <trd>fejv&aacute;nkos</trd>
  </trdgrp>
  <trd lng="id"><ind>bantal</ind> kepala</trd>
  <trd lng="ja">&#26517;  <pr>&#12414;&#12367;&#12425;</pr></trd>
  <trd lng="nl">hoofdkussen</trd>
  <trd lng="pt">travesseiro</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_g;&c_o;&c_l;&c_o;&c_v;&c_mol;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_u;&c_sh;&c_k;&c_a;<klr tip="amb"> (&c_p;&c_o;&c_d; &c_g;&c_o;&c_l;&c_o;&c_v;&c_u;)</klr></trd>
  </trdgrp>
  <trd lng="sk">vank&uacute;&scaron;</trd>
  <trd lng="uk">&c_p;&c_o;&c_d;&c_u;&c_sh;&c_k;&c_a; &c_p;&c_ib;&c_d; &c_g;&c_o;&c_l;&c_o;&c_v;&c_u;</trd>
  <trdgrp lng="zh">
    <trd>&#26517;&#22836; <pr>zh&ecaron;ntou</pr></trd>,
    <trd>&#26517;&#38957; <pr>zh&ecaron;ntou</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kusen.0tego">
  <kap><tld/>tego, <var><kap><tld/>tegilo</kap></var>,
  <var><kap><tld/>sako</kap></var></kap>
  <snc>
    <dif>
      &Scirc;an&gcirc;ebla &scirc;tofa sako, en kiun oni metas kusenon.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_v;&c_a;&c_l;&c_a;&c_ch;&c_k;&c_a;</trd>
  <trd lng="bg">&c_k;&c_a;&c_l;&c_malmol;&c_f; &c_n;&c_a; &c_v;&c_malmol;&c_z;&c_g;&c_l;&c_a;&c_v;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="br">toagenn</trd>
  <trd lng="cs">povlak na pol&scaron;t&aacute;&rcaron;</trd>
  <trd lng="de">Kissenbezug</trd>
  <trdgrp lng="en">
    <trd>pillow case</trd>,
    <trd>cushion cover</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>funda de <ind>coj&iacute;n</ind></trd>,
    <trd>funda de <ind>almohada</ind></trd>
  </trdgrp>
  <trd lng="fi">tyynyliina</trd>
  <trd lng="fr">taie</trd>
  <trd lng="hu">p&aacute;rnahuzat</trd>
  <trd lng="id"><ind>sarung</ind> bantal</trd>
  <trd lng="it">federa</trd>
  <trdgrp lng="ja">
    <trd>&#12463;&#12483;&#12471;&#12519;&#12531;&#12459;&#12496;&#12540;</trd>,
    <trd>&#24231;&#24067;&#22243;&#12459;&#12496;&#12540;  <pr>&#12374;&#12406;&#12392;&#12435;&#12363;&#12400;&#12540;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>sloop</trd>,
    <trd>kussensloop</trd>
  </trdgrp>
  <trd lng="pl">poszewka</trd>
  <trd lng="pt">fronha</trd>
  <trd lng="ru">&c_n;&c_a;&c_v;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a;</trd>
  <trd lng="sk">oblie&ccaron;ka na vank&uacute;&scaron;</trd>
  <trdgrp lng="sv">
    <trd>&ouml;rngott</trd>,
    <trd>dynvar</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_a;&c_v;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_sh;&c_i;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#26517;&#22836;&#22871; <pr>zh&ecaron;ntout&agrave;o</pr></trd>,
    <trd>&#26517;&#38957;&#22871; <pr>zh&ecaron;ntout&agrave;o</pr></trd>,
    <trd>&#26517;&#22871; <pr>zh&ecaron;nt&agrave;o</pr></trd>
  </trdgrp>
</drv>
</art>
