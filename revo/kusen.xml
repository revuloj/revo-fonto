<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kusen.xml,v 1.35 2023/12/07 19:16:28 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kusen</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="kusen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Sako el &scirc;tofo a&ubreve; ledo, remburita per mola&jcirc;oj (plumoj, haregoj)
      por ke oni povu sin komfortigi sur &gcirc;i:
      <ekz>
        la ju&gcirc;istoj sidis en siaj apogse&gcirc;oj kaj havis malanta&ubreve; la kapo <tld/>ojn el
        lanugo
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i e&ccirc; ricevis la permeson dormi sur velura <tld/>o anta&ubreve; lia pordo
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la gasto sin etendis sur la benko kaj sub la kapon metis la sakon kiel <tld/>on
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tablo, la orazeno kaj bastono el sako</lok>
        </fnt>;
      </ekz>
      <ekz>
        la viro ka&scirc;is la viza&gcirc;on en la lit<tld/>o <klr>(vd <ref cel="kusen.kap0o">kapkuseno</ref>)</klr>
        kaj malla&ubreve;te &gcirc;emis
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> e&ccirc; donis al &scirc;i malgrandan <ind>sid<tld/>o</ind>n
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
        <trd lng="de">Sitzkissen</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        konscienco senmakula estas <tld/>o plej mola
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="puf.0o">pufo</ref>
  </snc>
  <snc mrk="kusen.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Io mola, pufa, simila al <tld/>o:
      <ekz>
        li ekglitis sur musko<tld/>o
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Pramaljuna Morlino</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trd lng="bg">&c_v;&c_malmol;&c_z;&c_g;&c_l;&c_a;&c_v;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="bo">&t_sa;&t_ngata;&t_sa;&t_tsheg;&t_ma;&t_ga;&t_naro;&t_tsheg;</trd>
  <trd lng="br">torchenn</trd>
  <trd lng="cs">pol&scaron;t&aacute;&rcaron;</trd>
  <trd lng="de">Kissen</trd>
  <trdgrp lng="en">
    <trd>cushion</trd>,
    <trd>throw pillow</trd>
  </trdgrp>
  <trd lng="es">coj&iacute;n</trd>
  <trd lng="fi">tyyny</trd>
  <trd lng="fr">coussin</trd>
  <trdgrp lng="hu">
    <trd>p&aacute;rna</trd>,
    <trd>v&aacute;nkos</trd>
  </trdgrp>
  <trd lng="id">bantal</trd>
  <trd lng="it">cuscino</trd>
  <trdgrp lng="ja">
    <trd>&#x30AF;&#x30C3;&#x30B7;&#x30E7;&#x30F3;</trd>,
    <trd>&#x5EA7;&#x5E03;&#x56E3;  <pr>&#x3056;&#x3076;&#x3068;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">kussen</trd>
  <trd lng="pl">poduszka</trd>
  <trd lng="pt">almofada</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>podhlavnica</trd>,
    <trd>podu&scaron;ka</trd>,
    <trd>vank&uacute;&scaron;</trd>
  </trdgrp>
  <trd lng="sv">kudde</trd>
  <trd lng="uk">&c_p;&c_o;&c_d;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x8444; <pr>zu&ograve;</pr></trd>,
    <trd>&#x6795; <pr>zh&ecaron;n</pr></trd>,
    <trd>&#x6795;&#x5934; <pr>zh&ecaron;ntou</pr></trd>,
    <trd>&#x6795;&#x982D; <pr>zh&ecaron;ntou</pr></trd>,
    <trd>&#x5EA7;&#x57AB; <pr>zu&ograve;di&agrave;n</pr></trd>,
    <trd>&#x5EA7;&#x588A; <pr>zu&ograve;di&agrave;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kusen.0ego">
  <kap><ofc>*</ofc><tld/>ego</kap>
  <snc>
    <dif>
      Longa sako el tolo, plenigita de plumoj, sur kiu oni povas ku&scirc;i:
      <ekz>
        kie ili prenis tiajn matracojn kaj <tld/>egojn? mi e&ccirc; ek&scirc;vitis
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        mankas la hejma <tld/>ego (kial tiaj ne ekzistas ekster Nordio?), sed la sofo
        plene ta&ubreve;gas kiel lito
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 3 (53)</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="matrac.0o.lita">matraco</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Polster</trd>,
    <trd>Federbett</trd>
  </trdgrp>
  <trd lng="en">bolster</trd>
  <trdgrp lng="fr">
    <trd>couette</trd>,
    <trd>lit de plumes</trd>
  </trdgrp>
  <trd lng="ja">&#x5E03;&#x56E3;  <pr>&#x3075;&#x3068;&#x3093;</pr></trd>
  <trd lng="uk">&c_p;&c_je;&c_r;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x62B1;&#x6795; <pr>b&agrave;ozh&ecaron;n</pr></trd>,
    <trd>&#x88AB;&#x5B50; <pr>b&egrave;izi</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kusen.kap0o">
  <kap>kap<tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="kusen.0o"><tld lit="K"/>o</ref> destinita
      por apogi la kapon, ekzemple por dormo:
      <ekz>
        la konsilistoj sidis en siaj se&gcirc;oj kun la molaj kap<tld/>oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        subite, apud la kap<tld/>o de la &scirc;ajne morta korpo aperis bela
        junulo
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 69a, p.
        784a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_u;&c_sh;&c_k;&c_a; <klr>(&c_p;&c_a;&c_d; &c_g;&c_a;&c_l;&c_a;&c_v;&c_u;)</klr></trd>
  <trd lng="bg">&c_v;&c_malmol;&c_z;&c_g;&c_l;&c_a;&c_v;&c_n;&c_i;&c_c;&c_a;</trd>
  <trdgrp lng="br">
    <trd>goubenner</trd>,
    <trd>treuzplueg</trd>
  </trdgrp>
  <trd lng="cs">pol&scaron;t&aacute;&rcaron;</trd>
  <trd lng="de">Kopfkissen</trd>
  <trd lng="en">pillow</trd>
  <trd lng="es">almohada</trd>
  <trdgrp lng="fr">
    <trd>oreiller</trd>,
    <trd>polochon</trd>,
    <trd>traversin</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fejp&aacute;rna</trd>,
    <trd>fejv&aacute;nkos</trd>
  </trdgrp>
  <trd lng="id"><ind>bantal</ind> kepala</trd>
  <trd lng="ja">&#x6795;  <pr>&#x307E;&#x304F;&#x3089;</pr></trd>
  <trd lng="nl">hoofdkussen</trd>
  <trd lng="pt">travesseiro</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_g;&c_o;&c_l;&c_o;&c_v;&c_mol;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_u;&c_sh;&c_k;&c_a;<klr tip="amb"> (&c_p;&c_o;&c_d; &c_g;&c_o;&c_l;&c_o;&c_v;&c_u;)</klr></trd>
  </trdgrp>
  <trd lng="sk">vank&uacute;&scaron;</trd>
  <trd lng="uk">&c_p;&c_o;&c_d;&c_u;&c_sh;&c_k;&c_a; &c_p;&c_ib;&c_d; &c_g;&c_o;&c_l;&c_o;&c_v;&c_u;</trd>
  <trdgrp lng="zh">
    <trd>&#x6795;&#x5934; <pr>zh&ecaron;ntou</pr></trd>,
    <trd>&#x6795;&#x982D; <pr>zh&ecaron;ntou</pr></trd>
  </trdgrp>
</drv>


</art>
