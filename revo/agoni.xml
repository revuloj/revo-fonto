<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>agoni</rad>/o
</kap>
<drv mrk="agoni.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Dolora lasta batalo de mortanto kontra&ubreve; la morto.
    </dif>
  </snc>
  <trd lng="de">Agonie</trd>
  <trd lng="es">agon&iacute;a</trd>
  <trd lng="fr">agonie</trd>
  <trd lng="nl">doodsstrijd</trd>
  <trd lng="pt">agonia</trd>
  <trd lng="ru">&c_a;&c_g;&c_o;&c_n;&c_i;&c_ja;</trd>
</drv>

<drv mrk="agoni.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti tuj mortonta:
      <ekz>
        ... saturis la kreska&jcirc;ojn jam <tld/>antajn de
        l'soifo
        <fnt>(<aut>Kabe</aut>: <vrk>La Faraono, 1a vol.</vrk>,
        <lok>&ccirc;ap. 5</lok>)</fnt>.
      </ekz>
    </dif>
    <trd lng="fr">agoniser</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Esti tuj fini&gcirc;onta:
      <ekz>
        la tago <tld/>is;
      </ekz>
      <ekz>
        tiu brila epoko <tld/>as.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kaduk.0a">kaduka</ref>,
      <ref cel="krepus.0o">krepusko</ref>,
      <ref cel="auxtun.0o">a&ubreve;tuno</ref>.
    </refgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;cliner</trd>,
      <trd>agoniser</trd>
    </trdgrp>
  </snc>
  <trd lng="es">agonizar</trd>
  <trd lng="nl">zieltogen</trd>
  <trd lng="pt">agonizar</trd>
  <trd lng="ru">&c_a;&c_g;&c_o;&c_n;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
