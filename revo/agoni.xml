<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: agoni.xml,v 1.31 2023/10/12 16:57:43 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>agoni</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="agoni.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Dolora lasta <ref tip="super" cel="batal.0o">batalo</ref> de 
      mortanto kontra&ubreve; la <ref cel="mort.0o.ekmalvivo">morto</ref>:
      <ekz>
        mortigu min, &ccirc;ar kaptis min <tld/>o, &ccirc;ar mia animo &gcirc;is nun ankora&ubreve; estas en mi
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 1:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        estante en <tld/>o, li pre&gcirc;is pli fervore, kaj lia &scirc;vito fari&gcirc;is kiel gutoj da
        sango
        <fnt>
          <bib>NT</bib>
          <lok>Luko 22:44</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia mano kun spasmo de <tld/>o per la rigidi&gcirc;antaj fingroj premis &scirc;ian manon
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_g;&c_o;&c_n;&c_ib;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>ag&oacute;nie</trd>,
    <trd>posledn&iacute; ta&zcaron;en&iacute;</trd>,
    <trd>smrteln&yacute; z&aacute;pas</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Agonie</trd>,
     <trd>Todeskampf</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>agony</trd>,
     <trd>death throes</trd>
  </trdgrp>
  <trd lng="es">agon&iacute;a</trd>
  <trd lng="fr">agonie</trd>
  <trd lng="he">&gimel;&samekh;&yod;&samekh;&he;</trd>
  <trdgrp lng="hu">
    <trd>haldokl&aacute;s</trd>,
    <trd>ag&oacute;nia</trd>
  </trdgrp>
  <trd lng="it">agonia</trd>
  <trdgrp lng="ja">
    <trd>&#x65AD;&#x672B;&#x9B54;  <pr>&#x3060;&#x3093;&#x307E;&#x3064;&#x307E;</pr></trd>,
    <trd>&#x82E6;&#x60B6;  <pr>&#x304F;&#x3082;&#x3093;</pr></trd>,
    <trd>&#x6700;&#x671F;  <pr>&#x3055;&#x3044;&#x3054;</pr></trd>,
    <trd>&#x7D42;&#x7109;  <pr>&#x3057;&#x3085;&#x3046;&#x3048;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">doodsstrijd</trd>
  <trd lng="pl">agonia</trd>
  <trd lng="pt">agonia</trd>
  <trd lng="ro">agonie</trd>
  <trd lng="ru">&c_a;&c_g;&c_o;&c_n;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>ag&oacute;nia</trd>,
    <trd>ag&oacute;nia</trd>,
    <trd>dokon&aacute;vanie</trd>,
    <trd>smrte&lcaron;n&yacute; z&aacute;pas</trd>
  </trdgrp>
  <trd lng="tr">can &ccedil;eki&scedil;me</trd>
  <trd lng="uk">&c_a;&c_g;&c_o;&c_n;&c_ib;&c_ja;</trd>
</drv>

<drv mrk="agoni.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti tuj mortonta:
      <ekz>
        kelkaj <tld/>is en doloro, kelkajn oni mortigis
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        tie ku&scirc;is viro sur la lito de morto kaj <tld/>is
        <fnt>
          <bib>ElektFab</bib>
          <lok>Kamarado Gaja</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[&#x2026;]</klr> saturis la kreska&jcirc;ojn jam <tld/>antajn  de
        l'&#x202F;soifo <klr>(<ref cel="velk.0i.malfresxigxi">velkantajn</ref>)</klr>
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 5</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd><klr>in </klr>Agonie <ind>liegen</ind></trd>,
      <trd><klr>im </klr>Todeskampf <ind>liegen</ind></trd>,
      <trd><klr>mit dem </klr>Tode <ind>ringen</ind></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>be dying</trd>,
      <trd>suffer the throes of death</trd>
    </trdgrp>
    <trd lng="fr">agoniser</trd>
    <trdgrp lng="hu">
      <trd>haldoklik</trd>,
      <trd>agoniz&aacute;l</trd>
    </trdgrp>
    <trd lng="tr">can &ccedil;eki&scedil;mek</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Esti tuj fini&gcirc;onta:
      <ekz>
        tiu brila epoko <tld/>as;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Jam e&ccirc; <tld/>as povra Ver', &parsl; sur ter' Ukraina la urso ku&scirc;as
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="https://www.monato.be/2014/011397.php">Kiel senkulpe trafi mezen de fratbu&ccirc;a milito</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve; &scirc;i, la kapitalismo <tld/>as kaj alvenos nova modelo
        <fnt>
          <bib>Kon11</bib>
          <aut>Rogener Pavinski</aut>
          <vrk><url ref="https://tekstaro.com/t?nomo=kontakto-2011-2019">Agenita Ameno: &leftquot;Mi ne hisas flagojn, nek ju&gcirc;as&rightquot;</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kaduk.0igxi">kaduki&gcirc;i</ref>
    </refgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;cliner</trd>,
      <trd>agoniser</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd><klr>in den </klr>letzten <ind>Z&uuml;gen liegen</ind></trd>,
      <trd><klr>am </klr>Ende <ind>sein</ind></trd>,
      <trd><klr>dem </klr>Untergang <ind>geweiht sein</ind></trd>,
      <trd><klr>seine </klr>Zeit <ind>hinter sich haben</ind></trd>
    </trdgrp>
    <trd lng="hu">v&eacute;g&eacute;t j&aacute;rja</trd>
  </snc>
  <trd lng="he">&lamed;&gimel;&samekh;&vav;&samekh;</trd>
  <trdgrp lng="ja">
    <trd>&#x65AD;&#x672B;&#x9B54;&#x306E;&#x82E6;&#x3057;&#x307F;&#x3092;&#x5473;&#x308F;&#x3046;  <pr>&#x3060;&#x3093;&#x307E;&#x3064;&#x307E;&#x306E;&#x304F;&#x308B;&#x3057;&#x307F;&#x3092;&#x3042;&#x3058;&#x308F;&#x3046;</pr></trd>,
    <trd>&#x6B7B;&#x306B;&#x82E6;&#x60B6;&#x3059;&#x308B;  <pr>&#x3057;&#x306B;&#x304F;&#x3082;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x6700;&#x5F8C;&#x304C;&#x8FEB;&#x308B;  <pr>&#x3055;&#x3044;&#x3054;&#x304C;&#x305B;&#x307E;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="kek">aakanak</trd>
  <trd lng="be">&c_a;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>b&yacute;t v agonii</trd>,
    <trd>z&aacute;pasit se smrt&iacute;</trd>
  </trdgrp>
  <trd lng="es">agonizar</trd>
  <trd lng="it">agonizzare</trd>
  <trd lng="nl">zieltogen</trd>
  <trd lng="pl">kona&cacute;</trd>
  <trd lng="pt">agonizar</trd>
  <trd lng="ro">agoniza</trd>
  <trd lng="ru">&c_a;&c_g;&c_o;&c_n;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>by&tcaron; v ag&oacute;nii</trd>,
    <trd>skon&aacute;va&tcaron;</trd>,
    <trd>umiera&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&c_g;&c_o;&c_n;&c_ib;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_u;&c_t;&c_i; &c_v; &c_a;&c_g;&c_o;&c_n;&c_ib;&c_ji;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
</art>
