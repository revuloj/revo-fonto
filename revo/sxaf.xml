<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>&scirc;af</rad>/o
</kap>
<drv mrk="sxaf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el la familio <ref tip="super" cel="boved.0oj">bovedoj</ref>
      (<trd lng="la">Ovis</trd>), kiun homoj bredas pro lano kaj viando.
    </dif>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Homo tro bona:
      <ekz>
        se vi faros vin <tld/>o, la lupoj vin
        man&gcirc;os<fnt>Z</fnt>.
      </ekz> 
    </dif>
  </snc>
  <snc num="3">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Homo blinde imitema:
      <ekz>
        <tld/>e sekvi la anta&ubreve;irantojn.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Schaf</trd>
  <trd lng="en">sheep</trd>
  <trd lng="nl">schaap</trd>
  <trd lng="fr">mouton</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_v;&c_c;&c_a;</trd>,
    <trd>&c_b;&c_a;&c_r;&c_a;&c_n;</trd>
  </trdgrp>
</drv>
<drv mrk="sxaf.Sx0o">
  <kap><tld lit="&Scirc;"/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Stelaro kaj signo de la <ref tip="malprt" cel="zodiak.0o">zodiako</ref>
      (<trd lng="la">Aries</trd>).
    </dif>
  </snc>
  <trd lng="de">Widder</trd>
  <trd lng="nl">ram</trd>
  <trd lng="pl">baran</trd>
  <trd lng="ru">&c_O;&c_v;&c_je;&c_n;</trd>
</drv>  
</art>
