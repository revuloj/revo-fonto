<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;af</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sxaf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sxaf.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el la familio 
      <ref tip="malprt" cel="boved.0oj">bovedoj</ref>
      (<trd lng="la">Ovis</trd>), kiun homoj bredas pro 
      <ref cel="lan.0o">lano</ref> kaj viando.
    </dif>
    <trd lng="he">&kaf;&bet;&shin;</trd>
    <trdgrp lng="id">
      <trd>domba</trd>,
      <trd>biri-biri</trd>
    </trdgrp>
    <trd lng="it">pecora</trd>
  </snc>
  <snc mrk="sxaf.0o.FIG_bona">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="hom.0o">Homo</ref> tro 
      <ref cel="bon.0a">bona:</ref>
      <ekz>se vi faros vin <tld/>o, la lupoj vin
        man&gcirc;os
    <fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
   <trd lng="it">agnello</trd>
  </snc>
  <snc mrk="sxaf.0o.FIG_sekvema">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="hom.0o">Homo</ref> blinde 
      <ref cel="imit.0i">imitema</ref>:
      <ekz>
        <tld/>e sekvi la anta&ubreve;irantojn.
      </ekz>
    </dif>
   <trdgrp lng="it">
    <trd>pecora</trd>, 
    <trd>pecorone</trd>
  </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_v;&c_je;&c_ch;&c_k;&c_a;</trd>, 
    <trd>&c_b;&c_a;&c_r;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="bg">&c_o;&c_v;&c_c;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>ovella</trd>,
    <trd>xai, anyell</trd>
  </trdgrp>
  <trd lng="de">Schaf</trd>
  <trd lng="el">&pi;&rho;&omikron;&beta;&alfa;&tau;&jota_ton;&ny;&alfa;</trd>
  <trd lng="en">sheep</trd>
  <trd lng="es">oveja</trd>
  <trd lng="fa">&f_g;&a_w;&a_s;&a_f;&a_n;&a_d;</trd>
  <trd lng="fr">mouton</trd>
  <trdgrp lng="hu">
    <trd>juh</trd>,
    <trd>birka</trd>
  </trdgrp>
  <trd lng="nl">schaap</trd>
  <trd lng="pt">ovelha</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_v;&c_c;&c_a;</trd>,
    <trd>&c_b;&c_a;&c_r;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="sv">f&aring;r</trd>
</drv>

<drv mrk="sxaf.Sx0o">
  <kap><tld lit="&Scirc;"/>o</kap>
  <mlg>Ari</mlg>
  <mlg>&#9800;</mlg>
  <snc mrk="sxaf.Sx0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="lst" cel="stel.0figuro"
      lst="voko:zodiakaj_signoj">Stelfiguro</ref> kaj 
      signo de la 
      <ref tip="malprt" cel="zodiak.0o">zodiako</ref>
      (<trd lng="la">Aries</trd>).
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_A;&c_v;&c_je;&c_n;</trd>, 
    <trd>&c_B;&c_a;&c_r;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="bg">&c_O;&c_v;&c_je;&c_n;</trd>
  <trd lng="ca">&Agrave;ries</trd>
  <trd lng="de">Widder</trd>
  <trd lng="el">&Kappa;&rho;&jota;&omikron_ton;&sigma_fina;</trd>
  <trd lng="es">Aries</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_r;&a_j; <ind>&a_H;&a_fatha;&a_m;&a_fatha;&a_l;</ind></trd>,
    <trd>&a_S;&a_w;&a_r;&a_t; &a_f;&a_l;&f_k;&f_y; <ind>&a_H;&a_fatha;&a_m;&a_fatha;&a_l;</ind></trd>
  </trdgrp>
  <trd lng="fr">B&eacute;lier</trd>
  <trd lng="he">&mem;&zayin;&lamed; &tet;&lamed;&he;</trd>
  <trd lng="hu">Kos</trd>
  <trdgrp lng="id">
    <trd>Aries</trd>,
    <trd>Hamal</trd>,
    <trd>Mesa</trd>
  </trdgrp>
  <trd lng="it">Ariete</trd>
  <trd lng="nl">Ram</trd>
  <trd lng="pt">&Aacute;ries</trd>
  <trd lng="pl">baran</trd>
  <trd lng="ru">&c_O;&c_v;&c_je;&c_n;</trd>
</drv>
</art>
