<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>&scirc;af</rad>/o
</kap>
<drv mrk="sxaf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el la familio <ref tip="super" cel="bov.0edoj">bovedoj</ref>
      (<trd lng="la">Ovis</trd>), kiun homoj bredas pro lano kaj viando.
    </dif>
  </snc>
  <snc num="2">
    <uzo tip="klr">(f)</uzo>
    <dif>
      Homo tro bona:
      <ekz>
        se vi faros vin <tld/>o, la lupoj vin
        man&gcirc;os<fnt>Z</fnt>.
      </ekz> 
    </dif>
  </snc>
  <snc num="3">
    <uzo tip="klr">(f)</uzo>
    <dif>
      Homo blinde imitema:
      <ekz>
        <tld/>e sekvi la anta&ubreve;irantojn.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Schaaf</trd>
  <trd lng="en">sheep</trd>
  <trd lng="fr">mouton</trd>
</drv>
<drv mrk="sxaf.Sx0o">
  <kap><tld lit="&Scirc;"/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Stelaro kaj signo de la <ref tip="malprt" cel="zodiak.0o">zodiako</ref>
      (<trd lng="la">Aries</trd>).
    </dif>
  </snc>
  <trd lng="de">Widder</trd>
</drv>  
</art>
