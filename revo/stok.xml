<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stok.xml,v 1.63 2021/06/18 04:35:59 revo Exp $">
<kap>
  <ofc>9</ofc><rad>stok</rad>/o <fnt><aut>P. Christaller</aut>,
  <vrk>Deutsch-Esperanto W&ouml;rterbuch</vrk>, <lok>1910</lok></fnt>
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="stok.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="stok.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Disponebla <ref tip="super" cel="kvant.0o">kvanto</ref> 
      da <ref cel="var.0o">varoj</ref>:
      <ekz>
        la ministro devigis Coca-Cola detrui sian tutan
        <tld/>on
        <fnt><aut>R. Rotsaert</aut>,
        <vrk>Dioksino-krizo</vrk>,
        <bib>Monato</bib>,
        <lok>1999:7, p. 12a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="af">winkel</trd>
  <trd lng="am">&#x1218;&#x12F0;&#x1265;&#x122D;</trd>
  <trd lng="ar">&#x645;&#x62A;&#x62C;&#x631;</trd>
  <trd lng="az">ma&#x11F;aza</trd>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_ja;&c_w;&c_n;&c_y; &c_z;&c_a;&c_p;&c_a;&c_s;</trd>,
    <trd>&c_t;&c_a;&c_v;&c_a;&c_r;&c_a;&c_w;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_z;&c_a;&c_p;&c_a;&c_s;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="bn">&#x9A6;&#x9CB;&#x995;&#x9BE;&#x9A8;</trd>
  <trd lng="bo">&#xF5A;&#xF7C;&#xF44;&#xF0B;&#xF41;&#xF44;&#xF0B;</trd>
  <trd lng="co">magazzini</trd>
  <trd lng="da">butik</trd>
  <trdgrp lng="de">
    <trd>Lager</trd>,
    <trd>Bestand</trd>,
    <trd>Vorrat</trd>
  </trdgrp>
  <trd lng="en">stock</trd>
  <trd lng="et">kauplus</trd>
  <trd lng="eu">denda</trd>
  <trdgrp lng="fr">
    <trd>provision</trd>,
    <trd>r&eacute;serve<klr> (provision)</klr></trd>,
    <trd>stock</trd>
  </trdgrp>
  <trd lng="fy">winkel</trd>
  <trd lng="gl">tenda</trd>
  <trd lng="gu">&#xAB8;&#xACD;&#xA9F;&#xACB;&#xAB0;</trd>
  <trd lng="hi">&#x926;&#x941;&#x915;&#x93E;&#x928;</trd>
  <trd lng="hr">trgovina</trd>
  <trdgrp lng="hu">
    <trd>&aacute;ruk&eacute;szlet</trd>,
    <trd>k&eacute;szlet</trd>
  </trdgrp>
  <trd lng="hy">&#x56D;&#x561;&#x576;&#x578;&#x582;&#x569;</trd>
  <trd lng="is">verslun</trd>
  <trd lng="ja">&#x5E97;</trd>
  <trd lng="jw">nyimpen</trd>
  <trd lng="ka">&#x10DB;&#x10D0;&#x10E6;&#x10D0;&#x10D6;&#x10D8;&#x10D0;</trd>
  <trd lng="km">&#x17A0;&#x17B6;&#x1784;</trd>
  <trd lng="ko">&#xC0C1;&#xC810;</trd>
  <trd lng="lo">&#xEAE;&#xEC9;&#xEB2;&#xE99;</trd>
  <trd lng="ml">&#xD38;&#xD4D;&#xD31;&#xD4D;&#xD31;&#xD4B;&#xD7C;</trd>
  <trd lng="mr">&#x938;&#x94D;&#x91F;&#x94B;&#x905;&#x930;</trd>
  <trd lng="my">&#x1005;&#x1010;&#x102D;&#x102F;&#x1038;&#x1006;&#x102D;&#x102F;&#x1004;&#x103A;</trd>
  <trd lng="ne">&#x938;&#x94D;&#x91F;&#x94B;&#x930;</trd>
  <trd lng="ny">sitolo</trd>
  <trd lng="pa">&#xA38;&#xA1F;&#xA4B;&#xA30;</trd>
  <trdgrp lng="pl">
    <trd>zas&oacute;b</trd>,
    <trd>zapas</trd>,
    <trd>stan</trd>
  </trdgrp>
  <trd lng="nl">voorraad</trd>
  <trd lng="ps">&#x67E;&#x644;&#x648;&#x631;&#x646;&#x681;&#x64A;</trd>
  <trd lng="ru">&c_n;&c_a;&c_l;&c_i;&c_ch;&c_n;&c_y;&c_j; <ind>&c_z;&c_a;&c_p;&c_a;&c_s;</ind> &c_t;&c_o;&c_v;&c_a;&c_r;&c_o;&c_v;</trd>
  <trd lng="sd">&#x627;&#x633;&#x67D;&#x648;&#x631;</trd>
  <trd lng="si">&#xD9C;&#xDB6;&#xDA9;&#xDCF;</trd>
  <trd lng="sq">dyqan</trd>
  <trd lng="ta">&#xB95;&#xB9F;&#xBC8;&#xBAF;&#xBBF;&#xBB2;&#xBCD;</trd>
  <trd lng="te">&#xC38;&#xC4D;&#xC1F;&#xC4B;&#xC30;&#xC4D;</trd>
  <trd lng="th">&#xE01;&#xE32;&#xE23;&#xE08;&#xE31;&#xE14;&#xE40;&#xE01;&#xE47;&#xE1A;</trd>
  <trd lng="tl">tindahan</trd>
  <trd lng="ur">&#x633;&#x679;&#x648;&#x631;</trd>
  <trd lng="vi">c&#x1EED;a h&#xE0;ng</trd>
  <trd lng="yi">&#x5E7;&#x5E8;&#x5D0;&#x5B8;&#x5DD;</trd>
  <trd lng="zu">esitolo</trd>
</drv>

<drv mrk="stok.0i">
  <kap><ofc>9</ofc><tld/>i <fnt><bib>PIV1</bib></fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="stok.0i.EKON">
    <dif>
      Arigi komerca&jcirc;ojn, proviza&jcirc;ojn en ta&ubreve;gajn
      ejojn:
      <ekz>
        &ccirc;u la fabrikistoj demandis al si mem, kiuj povos 
        a&ccirc;eti la <tld/>itajn a&ubreve;tojn, kiuj akumuli&gcirc;as 
        en la parkejoj
        <fnt><aut>G. R. Ledon</aut>, <vrk>Senutilaj
        sindikatoj</vrk>, <bib>Monato</bib>, <lok>1999:6, 
        p 5a</lok></fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_s;&c_k;&c_l;&c_a;&c_d;&c_i;&c_r;&c_a;&c_m;</trd>
  <trd lng="en">stock</trd>
  <trd lng="de">lagern</trd>
  <trdgrp lng="fr">
    <trd>emmagasiner</trd>,
    <trd>entreposer</trd>,
    <trd>th&eacute;sauriser</trd>,
    <trd>stocker</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&eacute;szletez</trd>,
    <trd>t&aacute;rol</trd>,
    <trd>felhalmoz</trd>
  </trdgrp>
  <trd lng="nl">op voorraad houden</trd>
  <trdgrp lng="pl">
    <trd>sk&lstroke;adowa&cacute;</trd>,
    <trd>robi&cacute; <ind>zapasy</ind></trd>,
    <trd>bra&cacute; co&sacute; na <ind>stan</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; <ind>&c_z;&c_a;&c_p;&c_a;&c_s;&c_y;</ind></trd>,
    <trd>&c_h;&c_r;&c_a;&c_n;&c_i;&c_t;&c_mol; &c_n;&c_a; &c_s;&c_k;&c_l;&c_a;&c_d;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="stok.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="stok.0ejo.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Loko, en kiu estas konservataj komerca&jcirc;oj a&ubreve; 
      proviza&jcirc;oj:
      <ekz>
        tiuspeca sonorileto kutime pendis en tureto sur la tegmento de 
        iu <tld/>ejo
        <fnt><aut>Saliko</aut>,
        <vrk>Kolekti sonorilojn</vrk>,
        <bib>Monato</bib>,
        <lok>2001:2, p. 24a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="magaze.0o.stokejo">magazeno<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_m;&c_b;&c_a;&c_r;</trd>,
    <trd>&c_s;&c_mol;&c_v;&c_ib;&c_r;&c_a;&c_n;</trd>,
    <trd>&c_k;&c_l;&c_je;&c_c;&c_mol;</trd>
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d; &c_z;&c_a; &c_s;&c_t;&c_o;&c_k;&c_i;</trd>,
    <trd>&c_m;&c_a;&c_g;&c_a;&c_z;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>warehouse</trd>,
    <trd>stock room</trd>,
    <trd>storage room</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Lager</trd>,
    <trd>Lagerhaus</trd>,
    <trd>Speicher</trd>,
    <trd>Magazin</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>magasin<klr> (r&eacute;serve)</klr></trd>,
    <trd>r&eacute;serve<klr> (lieu)</klr></trd>
    <trd>entrep&ocirc;t</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>rakt&aacute;r</trd>,
    <trd>t&aacute;rh&aacute;z</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>sk&lstroke;ad</trd>,
    <trd>magazyn</trd>,
    <trd>hurtownia</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;</trd>,
    <trd>&c_a;&c_m;&c_b;&c_a;&c_r;</trd>
  </trdgrp>
</drv>
</art>
