    <dif>
      Disponebla <ref tip="super" cel="proviz.0o.ajxo">provizo</ref> 
      da <ref cel="var.0o">varoj</ref>:
      <ekz>
        la <tld/>o devas egali la a&ccirc;etojn minus la vendoj
        <fnt>
          <bib>ChC</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>on de la jam pretaj libroj kaj la rajton pretigi novajn eldonojn a&ccirc;etis
        la &jcirc;usfondita Esperantista Centra Librejo
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        kun tia jam preparita <tld/>o, li povus vivi ankora&ubreve; dudek jarojn
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        necesas havi &scirc;talajn nervojn kaj neel&ccirc;erpeblan <tld/>on da pacienco
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        signi la vojon per glureklamiloj, da kiuj li <klr>[&#8230;]</klr> povos ricevi         
        <tld/>on en apuda oficejo
        <fnt>
          <bib>ChR</bib>
        </fnt>;
      </ekz>
      <ekz>
        la Universitato de <nom>Amsterdam</nom> <klr>[&#8230;]</klr> eldonis duvoluman katalogon pri sia
        <tld/>o de interlingvistika literaturo
        <fnt>
          <bib>EeP</bib>
        </fnt>.
      </ekz>
    </dif>    
  </snc>
