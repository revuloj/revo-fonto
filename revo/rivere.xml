<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rivere.xml,v 1.28 2024/04/14 05:46:55 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>riverenc</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="rivere.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="rivere.0i.MOR">
    <dif>
      Respekte &gcirc;entile movi la korpon
      <ref tip="super" cel="klin.0i.MOV">klinante</ref> la buston 
      a&ubreve; <ref tip="sub" cel="fleks.genu0i">genufleksante</ref> por saluti:
      <ekz>
        li turnas sin kaj vidas kiel servisto <tld/>as la sinjoron
        <fnt>
          <aut>V. Poljanov, trad. I. H. Krestanoff</aut>,
          <vrk>Morto</vrk>,
          <lok><url ref="http://bibliotekoj.org/literaturamondo/PDF/1923/LM%201923-09bc.pdf">
          Literatura Mondo, 1923-09</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la knabino <klr>[&#x2026;]</klr> sen la e&ccirc; plej malgranda konfuzi&gcirc;o, tre gracie
        <tld/>is al sia estonta instruantino
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ankora&ubreve; unufoje <tld/>is, montrante la blankajn &scirc;trumpojn
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kanceliero respekte <tld/>is la re&gcirc;inon
        <fnt>
          <bib>IKr</bib>
          <lok>Tri Musketeroj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> profunde <tld/>is tri fojojn, sed respondis al la re&gcirc;o kun
        soldata sinteno
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        homa ondo <klr>[&#x2026;]</klr> <tld/>is lin &gcirc;is la piedoj
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi ne vidis, ke anta&ubreve; <nom>Allah</nom> <tld/>as
        &ccirc;iu, kiu estas en la &ccirc;ieloj, kaj kiu estas
        sur la tero
        <fnt>
          <aut>trad. I. Chiussi</aut>,
          <vrk>La Nobla Korano</vrk>,
          <lok>1970</lok>
        </fnt>?
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="klin.ador0igxi">adorklini&gcirc;i</ref>,
      <ref cel="kouxtoux.0i">ko&ubreve;to&ubreve;i</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_l;&c_a;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_k;&c_l;&c_a;&c_n;&c_ib;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_k;&c_l;&c_a;&c_n;&c_ja;&c_m; &c_s;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_l;&c_a;&c_n;&c_ja;&c_m; &c_s;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>klan&ecaron;t se k poct&ecaron;</trd>,
    <trd>poklonit se <klr>(p&rcaron;ed)</klr></trd>
  </trdgrp>
  <trd lng="de"><klr>sich </klr>verbeugen</trd>
  <trdgrp lng="en">
    <trd>bow</trd>,
    <trd>curtsey</trd>,
    <trd>make obeisance</trd>,
    <trd>salaam</trd>
  </trdgrp>
  <trd lng="es">reverenciar</trd>
  <trdgrp lng="fr">
    <trd>s'<ind>incliner</ind><klr> (pour saluer)</klr></trd>,
    <trd>faire la <ind>r&eacute;v&eacute;rence</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>meghajol</trd>,
    <trd>tiszteleg</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x304A;&#x8F9E;&#x5100;&#x3092;&#x3059;&#x308B;  <pr>&#x304A;&#x3058;&#x304E;&#x3092;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x6700;&#x656C;&#x793C;&#x3059;&#x308B;  <pr>&#x3055;&#x3044;&#x3051;&#x3044;&#x308C;&#x3044;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">een <ind>buiging</ind> maken</trd>
  <trdgrp lng="pl">
    <trd>uk&lstroke;oni&cacute; si&eogonek;</trd>,
    <trd>k&lstroke;ania&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_l;&c_a;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_l;&c_o;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kla&ncaron;a&tcaron; sa <klr>(pri pozdrave)</klr></trd>,
    <trd>pokloni&tcaron; sa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_k;&c_l;&c_a;&c_n;&c_ja;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_o;&c_b;&c_i;&c_t;&c_i; &c_r;&c_je;&c_v;&c_je;&c_r;&c_a;&c_n;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5311; <pr>g&#x14D;ng</pr></trd>,
    <trd>&#x97A0;&#x8EAC; <pr>j&#x16B;g&#x14D;ng</pr></trd>,
    <trd>&#x6298;&#x8170; <pr>zh&#xE9;y&#x101;o</pr></trd>,
    <trd>&#x54C8;&#x8170; <pr>h&#x101;y&#x101;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rivere.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="rivere.0o.MOR">
    <dif>
      Ago de tiu, kiu <tld/>as:
      <ekz>
        la maljunulino salte levi&gcirc;is de la benko kaj refaris profundan <tld/>on
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        farinte per la kapo <tld/>on al la vicre&gcirc;o, li eniris duongalope
        <fnt>
          <bib>DKM</bib>
          <lok>&Ccirc;apitro LXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        se mi faru <tld/>on, mi elektas potencon
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="salut.klin0o">klinsaluto</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_k;&c_l;&c_o;&c_n;</trd>,
    <trd>&c_r;&c_e;&c_v;&c_e;&c_r;&c_a;&c_n;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_p;&c_o;&c_k;&c_l;&c_o;&c_n;</trd>,
    <trd>&c_r;&c_je;&c_v;&c_je;&c_r;&c_a;&c_n;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>poklona</trd>,
    <trd>&uacute;klon</trd>
  </trdgrp>
  <trd lng="de">Verbeugung</trd>
  <trdgrp lng="en">
    <trd>bow</trd>,
    <trd>curtsey</trd>,
    <trd>obeisance</trd>,
    <trd>salaam</trd>
  </trdgrp>
  <trd lng="es">reverencia</trd>
  <trd lng="fr">r&eacute;v&eacute;rence</trd>
  <trdgrp lng="hu">
    <trd>meghajl&aacute;s</trd>,
    <trd>tisztelg&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x304A;&#x8F9E;&#x5100;  <pr>&#x304A;&#x3058;&#x304E;</pr></trd>,
    <trd>&#x6700;&#x656C;&#x793C;  <pr>&#x3055;&#x3044;&#x3051;&#x3044;&#x308C;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">buiging</trd>
  <trd lng="pl">uk&lstroke;on</trd>
  <trd lng="pt">rever&ecirc;ncia</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_k;&c_l;&c_o;&c_n;</trd>,
    <trd>&c_r;&c_je;&c_v;&c_je;&c_r;&c_a;&c_n;&c_s;</trd>
  </trdgrp>
  <trd lng="sk">poklona</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&c_k;&c_l;&c_ib;&c_n;</trd>,
    <trd>&c_r;&c_je;&c_v;&c_je;&c_r;&c_a;&c_n;&c_s;</trd>
  </trdgrp>
</drv>
</art>
