<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>riverenc</rad>/o
</kap>
<drv mrk="rivere.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Respekte &gcirc;entila klini&gcirc;o de la busto por saluti iun a&ubreve; ion.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_k;&c_l;&c_o;&c_n;</trd>,
    <trd>&c_r;&c_e;&c_v;&c_e;&c_r;&c_a;&c_n;&c_s;</trd>
  </trdgrp>
  <trd lng="de">Verbeugung</trd>
  <trdgrp lng="hu">
    <trd>meghajl&aacute;s</trd>,
    <trd>tisztelg&eacute;s</trd>
  </trdgrp>
  <trd lng="nl">buiging</trd>
  <trd lng="pt">rever&ecirc;ncia</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_k;&c_l;&c_o;&c_n;</trd>,
    <trd>&c_r;&c_je;&c_v;&c_je;&c_r;&c_a;&c_n;&c_s;</trd>
  </trdgrp>
</drv>

<drv mrk="rivere.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_l;&c_a;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_k;&c_l;&c_a;&c_n;&c_ib;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de"><klr>sich </klr>verbeugen</trd>
  <trdgrp lng="hu">
    <trd>meghajol</trd>,
    <trd>tiszteleg</trd>
  </trdgrp>
  <trd lng="nl">een <ind>buiging</ind> maken</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_l;&c_a;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_l;&c_o;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
