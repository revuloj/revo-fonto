<art mrk="$Id: poplit.xml,v 1.15 2022/03/26 12:56:10 revo Exp $">
<kap>
  <rad>poplit</rad>/o
</kap>
<drv mrk="poplit.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">ANA</uzo>
    <dif>
      Parto de la <ref tip="malprt" cel="krur.0o.tuto">kruro</ref>,
      kiu estas malanta&ubreve; la artiko  de la <ref cel="genu.0o.ANA">genuo</ref>:
      <ekz>
        la suro estas muskola elstara&jcirc;o &ccirc;e la malanta&ubreve;a parto de la kruro kaj sub la
        genukavo a&ubreve; <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=128622">Suro (muskolo)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">podkolenn&iacute; jamka</trd>
  <trd lng="de">Kniekehle</trd>
  <trd lng="fr">jarret</trd>
  <trd lng="hu">t&eacute;rdhajlat</trd>
  <trdgrp lng="it">                                        
    <trd>poplite</trd>,                       
    <trd>cavit&agrave; poplitea</trd>,                                   
  </trdgrp>
  <trd lng="nl">knieholte</trd>
  <trdgrp lng="pl">                                        
    <trd>d&oacute;&lstroke; podkolanowy</trd>,                       
    <trd>podkolanek</trd>,                                   
  </trdgrp>
  <trd lng="pt">jarrete</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_k;&c_o;&c_l;&c_je;&c_n;&c_n;&c_a;&c_ja; &c_v;&c_p;&c_a;&c_d;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>podkolenn&aacute; jamka</trd>,
    <trd>z&aacute;kolenie</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x456;&#x434;&#x43A;&#x43E;&#x43B;&#x456;&#x43D;&#x43D;&#x430; &#x44F;&#x43C;&#x43A;&#x430;</trd>
</drv>
</art>
