<kap>
  <rad>kanot</rad>/o <fnt>Z</fnt>  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kanot.0o">
  <kap><tld/>o</kap>
  <snc mrk="kanot.0o.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      Tradicia plezur- a&ubreve;
      sport-<ref tip="super" cel="boat.0o.SHI">boat</ref>eto
      ordinare pelata per pagajo, remilo a&ubreve; velo:
      <ekz>
        longa <tld/>o, kies padeloj re&jcirc;etis la radiojn
        de la okcidenta suno, transiris la fluon de supre
        <fnt>
        <aut>J. London, trad. R. Rossetti</aut>,
        <vrk>Dio de liaj prapatroj</vrk>,
        <lok>Nica Literatura Revuo, 1956-03 &gcirc;is 04,
        numero 1/4, p. 133a-148a</lok>.
        </fnt>;
      </ekz>
      <ekz>
        oni povas lui <tld/>on por fi&scirc;hoki
        inter la kanejoj de la lago
        <fnt>
          <aut>trad. Kaptay J.</aut>,
          <vrk><url ref="http://www.eszperanto.hu/turismo/tata/tata.htm">
          La urbo Tata</url></vrk>,
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kajak.0o.SHI">kajako</ref>
    </refgrp>
<!--
  tre ver&scirc;ajne ne dampfer
  &jcirc;v, 2011-06-23
  <trd lng="de">Promenadendampfer</trd>
-->
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_j;&c_d;&c_a;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_o;&c_e;</trd>
  </trdgrp>
  <trd lng="ca">canot</trd>
  <trdgrp lng="cs">
    <trd>lo&dcaron;ka</trd>,
    <trd>&ccaron;lun</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cano&euml;</trd>,
    <trd>canot</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kajak</trd>,
    <trd>cs&oacute;nak</trd>
  </trdgrp>
  <trd lng="ja">&#x904A;&#x89A7;&#x30DC;&#x30FC;&#x30C8;  <pr>&#x3086;&#x3046;&#x3089;&#x3093;&#x307C;&#x30FC;&#x3068;</pr></trd>
  <trd lng="nl">plezierboot</trd>
  <trdgrp lng="pl">
    <trd>kanadyjka <klr>(&lstroke;&oacute;d&zacute;)</klr></trd>,
    <trd>kanu</trd>
  </trdgrp>   
  <trd lng="ro">canoe</trd>
  <trd lng="ru">&c_b;&c_a;&c_j;&c_d;&c_a;&c_r;&c_k;&c_a;</trd>
  <trd lng="sk">&ccaron;ln</trd>
  <trdgrp lng="uk">
    <trd>&#x447;&#x43E;&#x432;&#x435;&#x43D;</trd>,
    <trd>&#x43A;&#x430;&#x442;&#x435;&#x440;</trd>,
    <trd>&#x431;&#x430;&#x439;&#x434;&#x430;&#x440;&#x43A;&#x430;</trd>
  </trdgrp>
  <trd lng="zh">&#x5212;&#x5B50; <pr>hu&#xE1;zi</pr></trd>
</drv>
