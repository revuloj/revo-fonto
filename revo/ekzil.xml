<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>ekzil</rad>/i
</kap>
<drv mrk="ekzil.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Devigi iun foriri el sia patrujo, malpermesante, ke li tien revenu:
      <ekz>
        Atenanoj <tld/>is Temistoklon;
      </ekz>
      <ekz>
        neniun ni <tld/>u, &ccirc;ar malpia estas la <ind><tld/>o</ind>!
        <trdgrp lng="be">
          <trd>&c_v;&c_y;&c_g;&c_n;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
          <trd>&c_e;&c_k;&c_z;&c_y;&c_l;&c_mol;</trd>
        </trdgrp>
        <trd lng="fr">exil<klr tip="ind"> (acte, &eacute;tat)</klr></trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pel.el0i">elpeli</ref>,
      <ref cel="pusx.el0ifor0i">forpu&scirc;i</ref>,
      <ref cel="proskr.0i">proskribi</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_g;&c_n;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">verbannen</trd>
  <trd lng="en">exile</trd>
  <trd lng="es">exiliar</trd>
  <trd lng="fr">exiler</trd>
  <trdgrp lng="pt">
    <trd>exilar</trd>,
    <trd>banir</trd>,
    <trd>degredar</trd>,
    <trd>desterrar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_g;&c_n;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_s;&c_l;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_o;&c_s;&c_l;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>landsf&ouml;rvisa</trd>,
    <trd>utvisa</trd>
  </trdgrp>
</drv>
</art>
