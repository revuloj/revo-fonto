<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>gren</rad>/o
    </kap>
    <drv mrk="gren.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="gren.0o.planto">
        <dif>
          Kulturata planto el familio <ref tip="super" cel="poac.0oj">poacoj</ref>,
          kies amelri&ccirc;aj semoj estas panigeblaj
          (<refgrp tip="sub"><ref cel="tritik.0o">tritiko</ref>,
            <ref cel="aven.0o">aveno</ref>,
            <ref cel="maiz.0o">maizo</ref>,
            <ref cel="riz.0o">rizo</ref></refgrp> kc):
          <ekz>
            konsumi sian <tld/>on anta&ubreve; &gcirc;ia maturi&gcirc;o
            <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>  
            <klr>(anta&ubreve;elspezi estontajn enspezojn)</klr>;
          </ekz><ekz>
            turka <tld/>o <klr>(maizo)</klr>.
          </ekz>
        </dif>
        <rim>
          <tld lit="G"/>o estas termino agrokultura kaj ekonomia;
          pli &gcirc;enerale similajn vegeta&jcirc;ojn (anka&ubreve; sova&gcirc;ajn a&ubreve;
          nekulturatajn) oni iam nomas
          <ref tip="super" cel="cereal.0o">cerealoj</ref>.
        </rim>
        <trdgrp lng="ru">
          <trd>&c_h;&c_l;&c_je;&c_b;&c_a;</trd>,
          <trd>&c_z;&c_je;&c_r;&c_n;&c_o;&c_v;&c_y;&c_je;</trd>,
          <trd>&c_z;&c_je;&c_r;&c_n;&c_o;&c_v;&c_a;&c_ja; &c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_a;</trd></trdgrp>
      </snc>
      <snc mrk="gren.0o.grajno">
        <dif>
          &Scirc;utebla amaso da semoj de tiuj vegeta&jcirc;oj,
          <ref tip="prt" cel="grajn.0o">grajno</ref>.
        </dif>
        <trdgrp lng="ru">
          <trd>&c_h;&c_l;&c_je;&c_b; <klr>(&c_s;&c_o;&c_b;&c_i;&c_r;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;)</klr></trd>,
          <trd>&c_z;&c_je;&c_r;&c_n;&c_o; <klr>(&c_s;&c_o;&c_b;&c_i;&c_r;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;)</klr></trd></trdgrp>
      </snc>
      <trd lng="de">Getreide</trd>
      <trd lng="en">grain</trd>
    </drv>
    <drv mrk="gren.0ejo">
      <kap><tld/>ejo</kap>
      <snc>
        <dif>
          Parto de domo, kie oni konservas dra&scirc;itan <tld/>on.
        </dif>
      </snc>
      <trd lng="de">Kornkammer</trd>
      <trd lng="ru">&c_h;&c_l;&c_je;&c_b;&c_n;&c_y;&c_j; &c_a;&c_m;&c_b;&c_a;&c_r;</trd>
    </drv>
  </art>

