<kap>
<ofc>*</ofc>
<rad>gren</rad>/o
</kap>
<drv mrk="gren.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">AGR</uzo>
  <snc mrk="gren.0o.planto">
    <dif>
      Kulturata vegeta&jcirc;o el familio <ref tip="super" cel="poac.0oj">poacoj</ref>,
      kies amelri&ccirc;aj semoj estas mueleblaj je <ref cel="farun.0o">faruno</ref>
      (<refgrp tip="sub">
        <ref cel="tritik.0o">tritiko</ref>,
        <ref cel="aven.0o">aveno</ref>,
        <ref cel="maiz.0o">maizo</ref>,
        <ref cel="riz.0o">rizo</ref>
      </refgrp>
      kc):
      <ekz>
        <tld/>oj: tritiko, sekalo, hordeo, aveno, rizo, maizo, poligono, spelto, milio 
         k. t. p. en iliaj prilabora&jcirc;oj en formo de faruno, grio, makaronoj, vermi&ccirc;eloj, 
         pano, baka&jcirc;o kaj faruna&jcirc;o de diversaj specoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        konsumi sian <tld/>on anta&ubreve; &gcirc;ia maturi&gcirc;o
        <klr>(anta&ubreve;elspezi estontajn enspezojn)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        eliros fajro kaj <klr>[&#8230;]</klr> ekstermos <ref cel="garb.0o.AGR">garbo</ref>jn a&ubreve;
        starantan <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 22:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        sunbruligita <tld/>o anta&ubreve; la spiki&gcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 19:26</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur &ccirc;iuj kampoj &ccirc;irka&ubreve;e kreskis <tld/>o, sekalo, hordeo kaj aveno
        <fnt>
          <bib>Fab2</bib>
          <lok>Poligono</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la kampo ondi&gcirc;adis la <tld/>o simile al movi&gcirc;anta maro
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tie]</klr> trovi&gcirc;is granda <tld/>kampo, sed la <tld/>o jam de longe estis
        fortran&ccirc;ita, nur la nudaj sekaj <ref cel="stopl.0o">stoplo</ref>j elstaris
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="pajl.0o">pajlo</ref>
    <trdgrp lng="be">
      <trd>&c_z;&c_b;&c_o;&c_zh;&c_zh;&c_a;&c_v;&c_a;&c_ja; &c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;</trd>,
      <trd>&c_z;&c_b;&c_o;&c_zh;&c_zh;&c_a;</trd>,
      <trd>&c_z;&c_b;&c_a;&c_zh;&c_y;&c_n;&c_a;</trd>
    </trdgrp>
    <trd lng="ca">cereal</trd>
    <trdgrp lng="ru">
      <trd>&c_h;&c_l;&c_je;&c_b;&c_a;</trd>,
      <trd>&c_z;&c_je;&c_r;&c_n;&c_o;&c_v;&c_y;&c_je;</trd>,
      <trd>&c_z;&c_je;&c_r;&c_n;&c_o;&c_v;&c_a;&c_ja;
&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="gren.0o.grajno">
    <uzo tip="fak">KUI</uzo>
    <dif>
      &Scirc;utebla amaso da semoj de tiuj vegeta&jcirc;oj,
      <ref tip="prt" cel="grajn.0o.AGR">grajnoj</ref>:
      <ekz>
        kia <tld/>o, tia pano
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        li kolektis la tutan <tld/>on de la sep jaroj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 41:48</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi man&gcirc;os <tld/>on malnovan pasintjaran
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 26:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        treni sakojn da <tld/>o en la muelejon
        <fnt>
          <bib>ElektFab</bib>
          <lok>La vivolongo</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la kortoj, anta&ubreve; la domoj, sur plej eta disponebla spaco, la <ref
        cel="drasx.0i.AGR">dra&scirc;i</ref>ta <tld/>o
        estas dis&jcirc;etita por ke &gcirc;i seki&gcirc;u
        <fnt>
          <bib>Monato</bib>
          <aut>S. Maul</aut>
          <vrk><url ref="&Steloj;monato2/006831.html">La&ubreve; ritmo de la
          rizejoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Tomo ver&scirc;is duontason da senkremigita lakto en sian bovlon de malgrasa
        matenman&gcirc;a <ind><tld/>a&jcirc;o</ind> (<ref cel="musli.0o">muslio</ref>, 
        <ref cel="cereal.0o">cereala&jcirc;o</ref>)
        <fnt>
          <vrk><url ref="https://glosbe.com/eo/en/malgrasa">malgrasa | Glosbe</url></vrk>
        </fnt>.
        <trd lng="de">Fr&uuml;hst&uuml;cksflocken</trd>
        <trd lng="en">breakfast <ind>cereal</ind></trd>
      </ekz>      
    </dif>
    <ref tip="sin" cel="cereal.0o">cerealo</ref>
    <trdgrp lng="be">
      <trd>&c_z;&c_b;&c_o;&c_zh;&c_zh;&c_a;</trd>,
      <trd>&c_z;&c_je;&c_r;&c_n;&c_je;</trd>,
      <trd>&c_h;&c_l;&c_je;&c_b;</trd>
    </trdgrp>
    <trd lng="ca">gra</trd>
    <trdgrp lng="ru">
      <trd>&c_h;&c_l;&c_je;&c_b; <klr>(&c_s;&c_o;&c_b;&c_i;&c_r;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;)</klr></trd>,
      <trd>&c_z;&c_je;&c_r;&c_n;&c_o; <klr>(&c_s;&c_o;&c_b;&c_i;&c_r;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="af">koring</trd>
  <trd lng="am">&#4704;&#4678;&#x120E;</trd>
  <trd lng="ar">&a_A;&a_l;&a_dh;&a_r;&a_t_marbuta;</trd>
