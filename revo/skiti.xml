<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: skiti.xml,v 1.13 2023/09/08 22:26:48 revo Exp $">
<kap>
  <rad>Skiti</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="skiti.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="skit.0ujo">Skitujo</ref>:
      <ekz>
        Aleksandro la Granda <klr>[&#8230;]</klr> iris al Medio kaj <tld/>o,
        kaptis Heraton kaj Samarkandon kaj da&ubreve;rigis al Hindio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=11964">Aleksandro
          la Granda</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_S;&c_k;&c_ib;&c_f;&c_ib;&c_ja;</trd>
  <trd lng="ca">Esc&iacute;tia</trd>
  <trd lng="cs">Skythie</trd>
  <trd lng="de">Scythien</trd>
  <trd lng="en">Scythia</trd>
  <trd lng="es">Escitia</trd>
  <trd lng="fr">Scythie</trd>
  <trd lng="hu">szk&iacute;tia</trd>
  <trd lng="pl">Scytia</trd>
  <trd lng="ru">&c_S;&c_k;&c_i;&c_f;&c_i;&c_ja;</trd>
  <trd lng="sk">Sk&yacute;tia</trd>
  <trd lng="uk">&c_S;&#x43A;&c_ib;&c_f;&c_ib;&#x44F; (&c_r;&c_je;&c_gu;&c_ib;&#x43E;&#x43D;)</trd>
</drv>
</art>
