<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: astm.xml,v 1.26 2024/09/05 01:49:43 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>astm</rad>/o
</kap>

<drv mrk="astm.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">Malsano</ref> karakterizata de
      malfacila
      spirado kun konvulsioj de la
      spiraj muskoloj:
      <ekz>
        diversaj pulmaj malsanoj, kiel ekz. <tld/>o<fnt><aut>Krys
        Ungar</aut>: <vrk>Vivu vigle kaj vegetare!</vrk>,
        <bib>Monato</bib><lok>jaro 1995a, numero 7a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_t;&c_m;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>astma</trd>,
    <trd>du&scaron;nost</trd>,
    <trd>z&#xE1;ducha</trd>
  </trdgrp>
  <trd lng="de">Asthma</trd>
  <trd lng="en">asthma</trd>
  <trd lng="es">asma</trd>
  <trd lng="fr">asthme</trd>
  <trd lng="he">&#x5D0;&#x5B7;&#x5E1;&#x5B0;&#x5EA;&#x5B0;&#x5DE;&#x5B8;&#x5D4;</trd>
  <trd lng="hu">asztma</trd>
  <trd lng="id">asma</trd>
  <trd lng="it">asma</trd>
  <trd lng="ja">&#21912;&#x606F;  <pr>&#x305C;&#12435;&#x305D;&#x304F;</pr></trd>
  <trdgrp lng="nl">
    <trd>aamborstigheid</trd>,
    <trd>astma</trd>
  </trdgrp>
  <trd lng="pl">astma</trd>
  <trd lng="ro">astm</trd>
  <trd lng="ru">&c_a;&c_s;&c_t;&c_m;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>astma</trd>,
    <trd>d&#xFD;chavica</trd>,
    <trd>z&#xE1;duch</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&c_s;&c_t;&#x43C;&c_a;</trd>,
    <trd>&#x44F;&c_d;&c_u;&c_h;&c_a;</trd>
  </trdgrp>
</drv>
</art>
