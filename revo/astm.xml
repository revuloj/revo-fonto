<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>astm</rad>/o
</kap>

<drv mrk="astm.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="san.mal0o.MED"
      lst="voko:malsanoj">Malsano</ref> karakterizata de malfacila
      spirado kun konvulsioj de la
      spiraj muskoloj:
      <ekz>
        diversaj pulmaj malsanoj, kiel ekz. <tld/>o<fnt><aut>Krys
        Ungar</aut>: <vrk>Vivu vigle kaj vegetare!</vrk>,
        <bib>Monato</bib><lok>jaro 1995a, numero 7a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_t;&c_m;&c_a;</trd>
  <trd lng="de">Asthma</trd>
  <trd lng="en">asthma</trd>
  <trd lng="es">asma</trd>
  <trd lng="fr">asthme</trd>
  <trd lng="hu">asztma</trd>
  <trd lng="it">asma</trd>
  <trdgrp lng="nl">
    <trd>aamborstigheid</trd>,
    <trd>astma</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_s;&c_t;&c_m;&c_a;</trd>
  <trd lng="sk">astma</trd>
</drv>
</art>
