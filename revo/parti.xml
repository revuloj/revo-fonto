    <trd>neobiectiv</trd>,
    <trd>p&abreve;rtinitor</trd>,
    <trd>subiectiv</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>jednostrann&yacute;</trd>,
    <trd>stran&iacute;cky</trd>,
    <trd>zaujat&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_a;&c_r;&c_t;&c_ib;&c_j;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_u;&#x43F;&c_je;&c_r;&c_je;&c_d;&c_zh;&c_je;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
