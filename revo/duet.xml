<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: duet.xml,v 1.18 2022/03/16 11:42:21 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>duet</rad>/o
</kap>
<drv mrk="duet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Muzika <ref tip="super" cel="kompon.0ajxo">kompona&jcirc;o</ref>
      por du vo&ccirc;oj a&ubreve; instrumentoj:
      <ekz>
        nun ili kantu kune! kia belega <tld/>o &gcirc;i estos!
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_u;&c_e;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>duet</trd>,
    <trd>dueto</trd>,
    <trd>duo</trd>,
    <trd>dvojzp&ecaron;v</trd>
  </trdgrp>
  <trd lng="de">Duett</trd>
  <trd lng="en">duet</trd>
  <trd lng="es">dueto</trd>
  <trd lng="fr">duo</trd>
  <trdgrp lng="hu">
    <trd>kett&odblac;s</trd>,
    <trd>duett</trd>
  </trdgrp>
  <trd lng="nl">duet</trd>
  <trd lng="pl">duet</trd>
  <trd lng="pt">dueto</trd>
  <trd lng="ru">&c_d;&c_u;&c_e;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>duet</trd>,
    <trd>dvojspev</trd>
  </trdgrp>
  <trd lng="uk">&#x434;&#x443;&#x435;&#x442;</trd>
</drv>
</art>
