<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: duet.xml,v 1.22 2024/07/04 09:30:33 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>duet</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="duet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Muzika <ref tip="super" cel="kompon.0ajxo">kompona&jcirc;o</ref>
      por du vo&ccirc;oj a&ubreve; instrumentoj:
      <ekz>
        nun ili kantu kune! kia belega <tld/>o &gcirc;i estos!
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz>
      <ekz>
        bela <tld/>o sonas de supre
        <fnt>
          <bib>IdO</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ili respondis gaje, en <tld/>o
        <fnt>
          <bib>TAA</bib>
          <lok>&Ccirc;apitro XII.</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_d;&c_u;&c_e;&c_t; <klr>(&c_t;&c_v;&c_o;&c_r;)</klr></trd>
    <trdgrp lng="cs">
      <trd>duet</trd>,
      <trd>dueto</trd>,
      <trd>duo</trd>,
      <trd>dvojzp&ecaron;v</trd>
    </trdgrp>
    <trd lng="de">Duett</trd>
    <trd lng="en">duet</trd>
    <trd lng="es">dueto</trd>
    <trd lng="fr">duo</trd>
    <trdgrp lng="hu">
      <trd>kett&odblac;s</trd>,
      <trd>duett</trd>
    </trdgrp>
    <trdgrp lng="ja">
      <trd>&#12487;&#12517;&#12456;&#12483;&#12488;  <pr>&#12391;&#12421;&#12360;&#12387;&#12392;</pr></trd>,
      <trd>&#20108;&#37325;&#22863;&#26354;  <pr>&#12405;&#12383;&#12376;&#12421;&#12358;&#12381;&#12358;&#12365;&#12423;&#12367;</pr></trd>,
      <trd>&#20108;&#37325;&#21809;&#26354;  <pr>&#12405;&#12383;&#12376;&#12421;&#12358;&#12375;&#12423;&#12358;&#12365;&#12423;&#12367;</pr></trd>
    </trdgrp>
    <trd lng="nl">duet</trd>
    <trd lng="pl">duet</trd>
    <trd lng="pt">dueto</trd>
    <trd lng="ru">&c_d;&c_u;&c_e;&c_t;</trd>
    <trdgrp lng="sk">
      <trd>duet</trd>,
      <trd>dvojspev</trd>
    </trdgrp>
    <trd lng="uk">&c_d;&c_u;&c_je;&c_t;</trd>
  </snc>
  <snc mrk="duet.0o.paro">
    <dif>
      <ref tip="super" cel="du.0o.paro">Duo</ref> muzikanta:
      <ekz>
        furoris la <tld/>o <nom>&Jcirc;omart</nom>&amp;<nom>Nata&scirc;a</nom>
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470; 10</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_d;&c_u;&c_e;&c_t; <klr>(&c_g;&c_u;&c_r;&c_t;)</klr></trd>
    <trd lng="de">Duo</trd>
  </snc>
</drv>
</art>
