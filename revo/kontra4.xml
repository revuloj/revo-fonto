<art mrk="$Id: kontra4.xml,v 1.16 2023/12/03 09:58:57 revo Exp $">
<kap>
  <rad>kontraktur</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kontra4.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="kontra4.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Stato, en kiu muskoloj restas senvole plimalpli da&ubreve;re
      stre&ccirc;itaj;
      <ref tip="sin" cel="rigid.strecx0eco">stre&ccirc;rigideco</ref>:
      <ekz>
        tetano ka&ubreve;zas fortan <tld/>on;
      </ekz>
      <ekz>
        &scirc;i elmontris iom diversajn malordojn: <tld/>o kaj anestezio de amba&ubreve; dekstraj
        ekstrema&jcirc;oj
        <fnt>
          <aut>C. Baudouin</aut>
          <vrk>La Arto de Memdisciplino: Psikagogio</vrk>
          <lok>1926</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="konvul.0o">konvulsio</ref>,
    <ref cel="kramf.0o">kramfo</ref>,
    <ref cel="spasm.0o">spasmo</ref>
  </refgrp>
  <trd lng="be">&c_k;&c_a;&c_n;&c_t;&c_r;&c_a;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>
  <trd lng="bg">&c_t;&c_je;&c_t;&c_a;&c_n;&c_i;&c_ja;</trd>
  <trd lng="ca">contractura</trd>
  <trdgrp lng="de">
     <trd>Kontraktur</trd>,
     <trd>dauerhafte <ind>Muskelverk&uuml;rzung</ind></trd>,
     <trd>Tetanie</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>contracture</trd>,
    <trd>t&eacute;tanie</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kontrakt&uacute;ra</trd>,
    <trd>izomzsugrod&aacute;s</trd>,
    <trd>izomr&ouml;vid&uuml;l&eacute;s</trd>
  </trdgrp>
  <trd lng="ja">&#x62D8;&#x7E2E;  <pr>&#x3053;&#x3046;&#x3057;&#x3085;&#x304F;</pr></trd>
  <trd lng="nl">kramp</trd> 
  <trdgrp lng="pl">
    <trd>kontraktura</trd>,
    <trd>mi&eogonek;&sacute;ni przykurcz</trd>
  </trdgrp>    
  <trd lng="pt">contratura</trd>
  <trd lng="ro">contrac&#x21B;ie</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_t;&c_r;&c_a;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>
  <trd lng="uk">&c_k;&c_o;&c_n;&c_t;&c_r;&c_a;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>
  <trd lng="zh">&#x631B;&#x7F29; <pr>lu&#xE1;nsu&#x14D;</pr></trd>
</drv>
</art>

