<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nervur.xml,v 1.20 2024/02/04 09:20:02 revo Exp $">
<kap>
  <rad>nervur</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="nervur.0o">
  <kap><tld/>o</kap>
  <snc mrk="nervur.0o.ripajxo">
    <dif>
      Arka, ripsimila parto de elstara disbran&ccirc;i&gcirc;anta strukturo.
    </dif>
    <subsnc>
    <uzo tip="fak">ARKI</uzo>
      <dif>
        Elstara arka <ref tip="super" cel="modlur.0o">modluro</ref>:
        <ekz>
          la <tld/>oj de gotika arka&jcirc;o;
        </ekz>
        <ekz>
          la blua kupolo de la ma&ubreve;zoleo <klr>[&#8230;]</klr> nombras 64 <tld/>ojn
          <fnt>
            <bib>Viki</bib>
            <lok><url ref="https://eo.wikipedia.org/?curid=15943"
                >Samarkando</url></lok>
          </fnt>.
        </ekz>
      </dif>
      <ref tip="vid" cel="rip.0o.FIG">ripo<sncref/></ref>
      <bld
        lok="&WCU;/thumb/b/b7/St._Stephanus_(Kornelim&uuml;nster)_24.jpg/160px-St._Stephanus_(Kornelim&uuml;nster)_24.jpg"
        prm="CC BY-SA 4.0"
        ><tld/>oj subportataj de kolono
          <fnt>
            <aut>Thomas Hummel, propra verko</aut>
            <lok><url ref="&WCW;/File:St._Stephanus_(Kornelim%C3%BCnster)_24.jpg"
                >https://commons.wikimedia.org/wiki/File:St._Stephanus_(Kornelim&uuml;nster)_24.jpg</url></lok>
          </fnt>
      </bld>
      <trd lng="de">Gew&ouml;lberippe</trd>
      <trd lng="hu">borda<klr tip="ind">
      (&eacute;p&iacute;t&eacute;szetben)</klr></trd>
      <trd lng="pl">&zdot;ebro <klr>(sklepienia)</klr></trd>
      <trdgrp lng="ru">
        <trd>&c_n;&c_je;&c_r;&c_v;&c_ju;&c_r;&c_a;</trd>,
        <trd>&c_r;&c_je;&c_b;&c_r;&c_o;
          &c_zh;&c_jo;&c_s;&c_t;&c_k;&c_o;&c_s;&c_t;&c_i;</trd>
      </trdgrp>
    </subsnc>
    <subsnc>
      <uzo tip="fak">ZOO</uzo><uzo tip="fak">BOT</uzo>
      <dif>
        Elstara tubeto &ccirc;e folio a&ubreve; flugilo de insekto:
        <ekz>
          ricino <klr>[&#8230;]</klr> kun blankaj <tld/>oj
          <fnt>
            <bib>Viki</bib>
            <lok><url ref="https://eo.wikipedia.org/?curid=20155"
                >Ricino</url></lok>
          </fnt>;
        </ekz>
        <ekz>
          la vejneca aran&gcirc;o de <tld/>oj estas videbla en la flugiloj de mu&scirc;oj kaj abeloj
          <fnt>
            <aut>Karolo Darvino, trad. Klivo Lendon</aut>
            <vrk><url ref="https://eduinf.waw.pl/esp/lern/librejo/0360/0017.php"
                >La Origino de Specioj</url></vrk>
            <lok>Glosaro</lok>
          </fnt>.
        </ekz>
      </dif>
      <ref tip="vid" cel="vaskul.0o">vaskulo</ref>
      <trdgrp lng="de">
        <trd>Blattader</trd>,
        <trd>Ader <klr>(Netzfl&uuml;gel)</klr></trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>rib</trd>,
        <trd>veining</trd>
      </trdgrp>
      <trd lng="hu">erezet<klr tip="ind"> (lev&eacute;len)</klr></trd>
      <trd lng="nl">bladnerf</trd>
      <trd lng="pl">unerwienie</trd>
      <trd lng="ru">&c_p;&c_r;&c_o;&c_zh;&c_i;&c_l;&c_k;&c_a;</trd>
    </subsnc>
    <!-- &ccirc;ar temas pri unuopa, devus esti sinonimo de nervo,
      mi do forkemntas tiun &ccirc;i sencon. WD
      <subsnc>
        <uzo tip="fak">ANA</uzo>
        <dif>
          Nervretaro.
        </dif>
        <refgrp tip="prt">
          <ref cel="nerv.0o">nervo</ref>,
          <ref cel="neuxron.0o">ne&ubreve;rono</ref>
        </refgrp>
        <trdgrp lng="de">
          <trd>Nervennetz</trd>,
          <trd>neuronales Netz</trd>
        </trdgrp>
        <trd lng="hu">erezet<klr tip="ind">
    (&eacute;rh&aacute;l&oacute;zat)</klr></trd>
        <trd lng="pl">unerwienie</trd>
      </subsnc>
    -->
  </snc>
  <trdgrp lng="cs">
    <trd>nervatura</trd>,
    <trd>&zcaron;ilnatina</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ader <klr>(einer &Auml;derung)</klr></trd>,
    <trd>Nerv <klr>(einer Nervatur)</klr></trd>,
    <trd>Rippe <klr>(einer Rippung)</klr></trd>
  </trdgrp>
  <trd lng="fr">nervure</trd>
  <trd lng="it">nervatura</trd>
  <trdgrp lng="ja">
    <trd>&#32709;&#33032;  <pr>&#12375;&#12415;&#12419;&#12367;</pr></trd>,
    <trd>&#33865;&#33032;  <pr>&#12424;&#12358;&#12415;&#12419;&#12367;</pr></trd>,
    <trd>&#12522;&#12502;</trd>,
    <trd>&#32907;&#26448;  <pr>&#12429;&#12367;&#12374;&#12356;</pr></trd>,
    <trd>&#26684;&#32257;  <pr>&#12363;&#12367;&#12360;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="pl">unerwienie</trd>
  <trd lng="pt">nervura</trd>
  <trdgrp lng="sk">
    <trd>nervat&uacute;ra</trd>,
    <trd>nervov&aacute; sie&tcaron;</trd>,
    <trd>&zcaron;ilnatina</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_zh;&c_i;&c_l;&c_k;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_zh;&c_i;&c_l;&c_k;&c_i;</trd>,
    <trd>&c_n;&c_je;&c_r;&c_v;&c_ju;&c_r;&c_a;</trd>,
    <trd>&c_r;&c_je;&c_b;&c_r;&c_o; &c_s;&c_k;&c_l;&c_je;&c_p;&c_ib;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
