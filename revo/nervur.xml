<art mrk="$Id: nervur.xml,v 1.17 2023/09/02 12:44:20 revo Exp $">
<kap>
  <rad>nervur</rad>/o
</kap>
<drv mrk="nervur.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">ARKI</uzo>
    <dif>
      Elstara ronda modluro:
      <ekz>
        la <tld/>oj de gotika arka&jcirc;o.
      </ekz>
    </dif>
    <trd lng="de">Rippe</trd>
    <trd lng="hu">borda<klr tip="ind">
(&eacute;p&iacute;t&eacute;szetben)</klr></trd>
    <trd lng="pl">&zdot;ebro <klr>(sklepienia)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_n;&c_je;&c_r;&c_v;&c_ju;&c_r;&c_a;</trd>,
      <trd>&c_r;&c_je;&c_b;&c_r;&c_o;
&c_zh;&c_jo;&c_s;&c_t;&c_k;&c_o;&c_s;&c_t;&c_i;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Elstara foliofibraro.
    </dif>
    <trdgrp lng="de">
      <trd>Rippe</trd>,
      <trd>Ader</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>rib</trd>,
      <trd>veining</trd>
    </trdgrp>
    <trd lng="hu">erezet<klr tip="ind"> (lev&eacute;len)</klr></trd>
    <trd lng="nl">bladnerf</trd>
    <trd lng="pl">unerwienie</trd>   
    <trd lng="ru">&c_p;&c_r;&c_o;&c_zh;&c_i;&c_l;&c_k;&c_a;</trd>
  </snc>
  <snc>
    <uzo tip="fak">ANA</uzo>
    <dif>
      Nervretaro.
    </dif>
    <trd lng="de">Ader</trd>
    <trd lng="hu">erezet<klr tip="ind">
(&eacute;rh&aacute;l&oacute;zat)</klr></trd>
    <trd lng="pl">unerwienie</trd>   
  </snc>
  <trdgrp lng="cs">
    <trd>nervatura</trd>,
    <trd>&zcaron;ilnatina</trd>
  </trdgrp>
  <trd lng="fr">nervure</trd>
  <trd lng="it">nervatura</trd>
  <trdgrp lng="ja">
    <trd>&#x7FC5;&#x8108;  [<ind>&#x3057;&#x307F;&#x3083;&#x304F;</ind>]</trd>,
    <trd>&#x8449;&#x8108;  [<ind>&#x3088;&#x3046;&#x307F;&#x3083;&#x304F;</ind>]</trd>,
    <trd>&#x30EA;&#x30D6;</trd>,
    <trd>&#x808B;&#x6750;  [<ind>&#x308D;&#x304F;&#x3056;&#x3044;</ind>]</trd>,
    <trd>&#x683C;&#x7E01;  [<ind>&#x304B;&#x304F;&#x3048;&#x3093;</ind>]</trd>
  </trdgrp>
  <trd lng="pl">unerwienie</trd>
  <trd lng="pt">nervura</trd>
  <trdgrp lng="sk">
    <trd>nervat&uacute;ra</trd>,
    <trd>nervov&aacute; sie&tcaron;</trd>,
    <trd>&zcaron;ilnatina</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x436;&#x438;&#x43B;&#x43A;&#x438;</trd>,
    <trd>&#x43F;&#x440;&#x43E;&#x436;&#x438;&#x43B;&#x43A;&#x438;</trd>,
    <trd>&#x43D;&#x435;&#x440;&#x432;&#x44E;&#x440;&#x430;</trd>,
    <trd>&#x440;&#x435;&#x431;&#x440;&#x43E; &#x441;&#x43A;&#x43B;&#x435;&#x43F;&#x456;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
