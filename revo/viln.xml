<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: viln.xml,v 1.7 2023/09/05 09:43:55 revo Exp $">
    <kap>
      <rad>Viln</rad>/o <fnt><bib>FK</bib></fnt>,
      <var><kap><rad var="ius">Vilnius</rad>/o</kap></var>
    </kap>

<drv mrk="viln.0o">
  <kap><tld/>o, <var><kap><tld var="ius"/>o</kap></var></kap>
  <uzo tip="fak">POL</uzo><uzo tip="fak">GEOG</uzo>
  <snc mrk="viln.0o.cxefurbo">
    <dif>
      <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&Ccirc;efurbo</ref>
      de <ref tip="malprt" cel="litov.L0ujo">Litovujo</ref>:
      <ekz>
        al ni <tld/>o trumpetas ordonojn: <nom>Olgerd</nom> falos Rusujon, <nom>Skirgell</nom> iros
        Polujon
        kaj princ' <nom>Kejstut</nom> atakos Te&ubreve;tonojn
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>  
      <ekz>
        litva pola poeto <nom>Mickievi&ccirc;</nom>, el <tld/>o forpelita de la rusa registaro, sonigis
        tra E&ubreve;ropo genian alvokon al sankti&gcirc;o de sia popolo por savi &ccirc;iujn gentojn de
        l' mondo
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>  
      <ekz>
        oni nostalgie, sed tute prave diras ke &leftquot;<tld/>o Jerusalemo estis&#x201D;
        <fnt>
          <bib>Monato</bib>
          <aut>Zofia Banet-Fornalowa</aut>
          <vrk><url ref="&Monato;/2009/009788.php">Muzeo pri la historio de pollandaj
          judoj</url></vrk>
        </fnt>;
      </ekz> 
      <ekz>
        la 90a Universala Kongreso en <tld var="ius"/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Last</aut>
          <vrk><url ref="&Monato;/2005/008652.php">Strando-frando</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la centro de fizika E&ubreve;ropo, de Atlantiko &gcirc;is Uralo, situas 
        proksime al <tld var="ius"/>o en Litovio
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2004/008334.html">Centra demando</url></vrk>
        </fnt>.
      </ekz> 
    </dif>
    <trdgrp lng="de">
      <trd>Vilnius</trd>,
      <trd>Wilna</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_V;&c_ib;&c_l;&c_mol;&c_n;&c_ja;</trd>,
    <trd>&c_V;&c_ib;&c_l;&c_mol;&c_n;&c_ju;&c_s;</trd>
  </trdgrp>
  <trd lng="ja">&#x30D3;&#x30EA;&#x30CB;&#x30E5;&#x30B9;</trd>
  <trd lng="uk">&c_V;&c_ib;&#x43B;&#x44C;&#x43D;&#x43E; (&c_s;&c_t;&#x43E;&#x43B;&c_i;&c_c;&#x44F;)</trd>
</drv>

  </art>

