<drv mrk="koz.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="koz.0o.KOMUNE">
    <dif>
      &Ccirc;ia <ref tip="vid" cel="afer.0o.cxio">afero</ref>,
      a&jcirc;o, koncerno esta a&ubreve; imagebla:
      <ekz>
        filozofio ser&ccirc;as la sencon a&ubreve; la celon de la <tld/>oj
        <fnt>
          <aut>V. O&scaron;lak</aut>,
          <vrk>Foje e&ccirc; pensi estas &gcirc;uo</vrk>,
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aper.0ajxo.fenomeno">apera&jcirc;o<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>chose</trd>,
    <trd>entit&eacute;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rzecz</trd>,
    <trd>istota</trd>,
    <trd>podmiot</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>lucru</trd>,
    <trd>fiind</trd>,
    <trd>subiect</trd>
  </trdgrp> 
</drv>
</art>

