<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>junk</rad>/o
</kap>
<drv mrk="junk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="junk.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Juncus</trd>), kreskanta en malsekaj grundoj,
      uzata por fari matojn, korbojn ktp.
    </dif>
  </snc>
  <trd lng="de">Binse</trd>
</drv>
<drv mrk="junk.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de unukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Juncaceae</trd>), herbsimilaj, kun rondaj, rektaj
      kaj flekseblaj trunketoj.
    </dif>
  </snc>
  <trd lng="de">Binsengew&auml;chse</trd>
</drv>
</art>
