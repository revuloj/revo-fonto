<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>junk</rad>/o
</kap>
<drv mrk="junk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="junk.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Juncus</trd>), kreskanta en malsekaj grundoj,
      uzata por fari matojn, korbojn ktp.
    </dif>
  </snc>
  <trd lng="de">Binse</trd>
  <trd lng="ru">&c_s;&c_i;&c_t;&c_n;&c_i;&c_k;</trd>
</drv>
<drv mrk="junk.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.unu0aj">unukotiledonaj</ref>
      plantoj (<trd lng="la">Juncaceae</trd>), herbsimilaj, kun rondaj, rektaj
      kaj flekseblaj trunketoj.
    </dif>
  </snc>
  <trd lng="de">Binsengew&auml;chse</trd>
</drv>
</art>
