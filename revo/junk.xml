<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: junk.xml,v 1.25 2022/04/30 08:42:43 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>junk</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="junk.0o">
  <kap><tld/>o</kap>
  <snc mrk="junk.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="kan.0o.BOT">Kano</ref> vivanta en
      malsekaj grundoj, havanta longajn, rondigitajn pintaj foliojn,
      tigojn kavajn a&ubreve; kun blanka spongeca interna&jcirc;o, 
      kun trinombraj floroj kaj trikameraj kapsuloj,
      uzata por fari matojn, korbojn ktp;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref> 
      el familio <ref tip="malprt" cel="junkac.0oj">junkacoj</ref>
      (<trd lng="la">Juncus</trd>):
      <ekz>
        &ccirc;u povas kreski <klr>[&#8230;]</klr> <tld/>o sen akvo?
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 8:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        li havis anta&ubreve; si la tutan rivereton kun la klara, fre&scirc;a akvo, kie la <tld/>oj
        klini&gcirc;adis tien kaj reen
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        li prenis sakon da perloj ka&scirc;itan en la <tld/>o kaj dirinte plu nenion fortrenis
        &gcirc;in
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino kaj Rozulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        flustras la <tld/>oj en a&ubreve;tunaj ventoj
        <fnt>
        <aut>P. Thorsen</aut>,
        <vrk><url ref="http://www.poulthorsen.dk/junkoj.htm">
        Dum flustras la junkoj</url></vrk>,
        <lok>Sennacieca Revuo, 1934</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fragmi.0o">fragmito</ref>,
      <ref cel="skirp.0o">skirpo</ref>
    </refgrp>
    <bld
      lok="&WCU;/thumb/4/4b/Illustration_Juncus_hostii0.jpg/457px-Illustration_Juncus_hostii0.jpg"
      prm="PD"
      >botanika tabulo pri du specioj de <tld/>o; A:
        Juncus effusus; B: Juncus trifidus subsp. hostii
        <fnt>
          <aut>Prof. Dr. Otto Wilhelm Thom&eacute; Flora von Deutschland, &Ouml;sterreich und der Schweiz and Kurt Stueber, http://www.biolib.de www.biolib.de</aut>
          <lok><url ref="&WCW;/File:Illustration_Juncus_hostii0.jpg"
              >&WCW;/File:Illustration_Juncus_hostii0.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_s;&c_ib;&c_t;</trd>
  <trdgrp lng="bg">
    <trd>&c_t;&c_r;&c_malmol;&c_s;&c_t;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_m;&c_malmol;&c_sh;</trd>
  </trdgrp>
  <trd lng="de">Binse</trd>
  <trd lng="el">&sigma;&chi;&omikron;&jota_ton;&ny;&omikron;&sigma_fina;</trd>
  <trd lng="en">rush<klr tip="amb"> (grassy plant)</klr></trd>
  <trd lng="es">junco</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_A;&a_z;&a_w;</trd>,
    <trd>&a_n;&f_y; &a_b;&a_w;&a_r;&f_y;&a_A;</trd>
  </trdgrp>
  <trd lng="fr">jonc</trd>
  <trd lng="hu">szitty&oacute;</trd>
  <trd lng="it">giunco</trd>
  <trd lng="nl">rus</trd>
  <trd lng="pl">sit</trd>
  <trd lng="pt">junco</trd>
  <trd lng="ru">&c_s;&c_i;&c_t;&c_n;&c_i;&c_k;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&c_t;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_o;&c_s;&c_i;&c_t;&c_n;&c_ja;&c_k;</trd>
  </trdgrp>
</drv>
</art>
