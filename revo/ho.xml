<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ho.xml,v 1.21 2020/12/18 17:10:20 revo Exp $">
<kap>
   <ofc>*</ofc> <rad>ho</rad> <fnt> <bib>PV</bib></fnt>,
   <var><kap><rad var="o">oh</rad> <fnt> <bib>IK</bib></fnt></kap></var>
</kap>
<drv mrk="ho.0">
   <kap><ofc>*</ofc><tld/>, <var><kap><tld var="o"/></kap></var></kap>
   <gra><vspec>ekkrio</vspec></gra>
   <snc mrk="ho.0.surprizo">
     <dif>
       Surprize, indigne, dolore!:
      <ekz>
        <tld/>, vi estas feli&ccirc;a homo
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>!
      </ekz>
      <ekz>
        <tld/> ve! mia malbeno alpelis lin al la morto
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>! &gcirc;i faris el mi okdekjaran homon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld var="o"/>, en ventro kia pezo, sentas &scirc;tonojn mi en mezo
        <fnt>
          <bib>ElektFab</bib>
          <lok>La sep kapridoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld var="o"/>, vi ne scias, kia estas la sorto de l' kamparano
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>!
      </ekz>
     </dif>
     <trd lng="es">oh</trd>
     <trdgrp lng="fi">
       <trd>voi</trd>,
       <trd>oi</trd>
     </trdgrp>
     <trd lng="it">oh</trd>
     <trd lng="pt">oh</trd>
     <trdgrp lng="ru">
       <trd>&c_o;</trd>,
       <trd>&c_o;&c_h;</trd>
     </trdgrp>
   </snc>
   <snc mrk="ho.0.alvoko">
     <dif>
      Atentigo al alparolato:
      <ekz>
        <tld/>, li estis persono tre prudenta
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>!
      </ekz>
      <ekz>
        <tld/> potenca senkorpa mistero
        <fnt>
          <aut>L. L. Zamenhof</aut>
          <vrk>Pre&gcirc;o sub la Verda Standardo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld var="o"/> ne, ne! &scirc;i ektremis pensante, ke Franjo a&ubreve; Sta&ccirc;jo povas veki&gcirc;i
        <fnt>
          <bib>IK</bib>
        </fnt>.
      </ekz>
     </dif>
     <trd lng="es">oh<klr> (vocativo)</klr></trd>
     <trdgrp lng="fi">
       <trd>hei</trd>,
       <trd>hoi</trd>,
       <trd>oi</trd>
     </trdgrp>
     <trd lng="it">o<klr> (vocativo)</klr></trd>
     <trd lng="pt">&oacute;</trd>
   </snc>
  <trdgrp lng="cs">
    <trd>jak&eacute; ne&scaron;t&ecaron;st&iacute;</trd>,
    <trd>jej</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>o</trd>,
     <trd>oh</trd>
   </trdgrp>
  <trdgrp lng="en">
     <trd>oh</trd>,
     <trd>whoa</trd>,
     <trd>wow</trd>,
     <trd>yikes</trd>,
     <trd>yow</trd>
   </trdgrp>
  <trdgrp lng="fr">
     <trd>ho</trd>,
     <trd>oh</trd>,
     <trd>&ocirc;</trd>
   </trdgrp>
  <trdgrp lng="hu">
     <trd>&oacute;</trd>,
     <trd>oh</trd>,
     <trd>ah</trd>
   </trdgrp>
  <trd lng="nl">o</trd>
  <trd lng="ru">&c_o;</trd>
  <trdgrp lng="sk">
    <trd>ak&eacute; ne&scaron;&tcaron;astie</trd>,
    <trd>och</trd>
  </trdgrp>
  <trd lng="tp">a</trd>
</drv>
</art>
