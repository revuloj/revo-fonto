<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ho</rad>
</kap>
<drv mrk="ho.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc mrk="ho.0.surprizo">
    <dif>
      Interj. esprimanta la surprizon, indignon, doloron:
      <ekz>
        <tld/>! kia ridinda vesto!
      </ekz>
      <ekz>
        <tld/> ve! li mortis.
      </ekz>
    </dif>
    <trd lng="pt">oh</trd>
    <trdgrp lng="ru">
      <trd>&c_o;</trd>,
      <trd>&c_o;&c_h;</trd>
    </trdgrp>
  </snc>
  <snc mrk="ho.0.alvoko">
    <dif>
      Interj. signifanta alvokon:
      <ekz>
        <tld/> vi, granda fonto de l' amo kaj vero<fnt>Z</fnt>!
      </ekz>
    </dif>
    <trd lng="pt">&oacute;</trd>
  </snc>
  <trdgrp lng="de">
    <trd>o</trd>,
    <trd>oh</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>oh</trd>,
    <trd>whoa</trd>,
    <trd>wow</trd>,
    <trd>yikes</trd>,
    <trd>yow</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ho</trd>,
    <trd>oh</trd>,
    <trd>&ocirc;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&oacute;</trd>,
    <trd>oh</trd>,
    <trd>ah</trd>
  </trdgrp>
  <trd lng="nl">o</trd>
  <trd lng="ru">&c_o;</trd>
</drv>
</art>
