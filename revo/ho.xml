<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ho</rad>
</kap>
<drv mrk="ho.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Interj. esprimanta la surprizon, indignon, doloron:
      <ekz>
        <tld/>! kia ridinda vesto!
      </ekz>
      <ekz>
        <tld/> ve! li mortis.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_o;</trd>,
      <trd>&c_o;&c_h;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Interj. signifanta alvokon:
      <ekz>
        <tld/> vi, granda fonto de l' amo kaj vero<fnt>Z</fnt>!
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>o</trd>,
    <trd>oh</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ho</trd>,
    <trd>oh</trd>,
    <trd>&ocirc;</trd>
  </trdgrp>
  <trd lng="nl">o</trd>
  <trd lng="ru">&c_o;</trd>
</drv>
</art>
