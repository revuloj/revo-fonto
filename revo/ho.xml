<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>ho</rad> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ho.0">
   <kap><ofc>*</ofc><tld/></kap>
   <gra><vspec>ekkrio</vspec></gra>
   <snc mrk="ho.0.surprizo">
     <dif>
       Surprize, indigne, dolore!:
       <ekz>
         <tld/>! kia ridinda vesto!
       </ekz>
       <ekz>
         <tld/> ve! li mortis.
       </ekz>
     </dif>
     <trd lng="es">oh</trd>
     <trdgrp lng="fi">
       <trd>voi</trd>,
       <trd>oi</trd>
     </trdgrp>
     <trd lng="it">oh</trd>
     <trd lng="pt">oh</trd>
     <trdgrp lng="ru">
       <trd>&c_o;</trd>,
       <trd>&c_o;&c_h;</trd>
     </trdgrp>
   </snc>
   <snc mrk="ho.0.alvoko">
     <dif>
       Atentigo al alparolato:
       <ekz>
         <tld/> vi, granda fonto de l' amo kaj vero<fnt>Z</fnt>!
       </ekz>
     </dif>
     <trd lng="es">oh<klr> (vocativo)</klr></trd>
     <trdgrp lng="fi">
       <trd>hei</trd>,
       <trd>hoi</trd>,
       <trd>oi</trd>
     </trdgrp>
     <trd lng="it">o<klr> (vocativo)</klr></trd>
     <trd lng="pt">&oacute;</trd>
   </snc>
  <trdgrp lng="cs">
    <trd>jaké neštěstí</trd>,
    <trd>jej</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>o</trd>,
     <trd>oh</trd>
   </trdgrp>
  <trdgrp lng="en">
     <trd>oh</trd>,
     <trd>whoa</trd>,
     <trd>wow</trd>,
     <trd>yikes</trd>,
     <trd>yow</trd>
   </trdgrp>
  <trdgrp lng="fr">
     <trd>ho</trd>,
     <trd>oh</trd>,
     <trd>&ocirc;</trd>
   </trdgrp>
  <trdgrp lng="hu">
     <trd>&oacute;</trd>,
     <trd>oh</trd>,
     <trd>ah</trd>
   </trdgrp>
  <trd lng="nl">o</trd>
  <trd lng="ru">&c_o;</trd>
  <trdgrp lng="sk">
    <trd>aké nešťastie</trd>,
    <trd>och</trd>
  </trdgrp>
  <trd lng="tp">a</trd>
</drv>
</art>
