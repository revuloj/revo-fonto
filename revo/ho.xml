<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ho.xml,v 1.81 2025/10/17 22:30:13 revo Exp $">
<kap>
   <ofc>*</ofc> <rad>ho</rad> <fnt> <bib>PV</bib></fnt>,
   <var><kap><rad var="o">oh</rad> <fnt> <bib>IK</bib></fnt></kap></var>
</kap>
<drv mrk="ho.0">
   <kap><ofc>*</ofc><tld/>, <var><kap><tld var="o"/></kap></var></kap>
   <gra><vspec>ekkrio</vspec></gra>
   <snc mrk="ho.0.surprizo">
     <dif>
       Surprize, indigne, dolore!:
      <ekz>
        <tld/>, vi estas feli&ccirc;a homo
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>!
      </ekz>
      <ekz>
        <tld/> ve! mia malbeno alpelis lin al la morto
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>! &gcirc;i faris el mi okdekjaran homon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld var="o"/>, en ventro kia pezo, sentas &scirc;tonojn mi en mezo
        <fnt>
          <bib>ElektFab</bib>
          <lok>La sep kapridoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld var="o"/>, vi ne scias, kia estas la sorto de l' kamparano
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>!
      </ekz>
     </dif>
     <trd lng="be">&c_v;&c_o;&c_h;</trd>
     <trd lng="ca">oh <klr tip="amb">(indicant sorpresa o dolor)</klr></trd>
     <trd lng="es">oh</trd>
     <trdgrp lng="fi">
       <trd>voi</trd>,
       <trd>oi</trd>
     </trdgrp>
     <trd lng="it">oh</trd>
     <trd lng="pt">oh</trd>
     <trdgrp lng="ru">
       <trd>&c_o;</trd>,
       <trd>&c_o;&c_h;</trd>
     </trdgrp>
   </snc>
   <snc mrk="ho.0.alvoko">
     <dif>
      Atentigo al alparolato:
      <ekz>
        <tld/>, li estis persono tre prudenta
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>!
      </ekz>
      <ekz>
        <tld/> potenca senkorpa mistero
        <fnt>
          <aut>L. L. Zamenhof</aut>
          <vrk>Pre&gcirc;o sub la Verda Standardo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld var="o"/> ne, ne! &scirc;i ektremis pensante, ke Franjo a&ubreve; Sta&ccirc;jo povas veki&gcirc;i
        <fnt>
          <bib>IK</bib>
        </fnt>.
      </ekz>
     </dif>
     <trd lng="be">&c_g;&c_e;&c_j;</trd>
     <trdgrp lng="ca">
       <trd>ho <klr tip="amb">(vocatiu)</klr></trd>,
       <trd>o <klr tip="amb">(vocatiu)</klr></trd>
     </trdgrp>
     <trd lng="es">oh<klr> (vocativo)</klr></trd>
     <trdgrp lng="fi">
       <trd>hei</trd>,
       <trd>hoi</trd>,
       <trd>oi</trd>
     </trdgrp>
     <trd lng="it">o<klr> (vocativo)</klr></trd>
     <trd lng="pt">&oacute;</trd>
  </snc>
  <ref tip="hom" cel="h.0.konsonanto">ho (litero)</ref>
  <trd lng="af">o</trd>
  <trd lng="am">&#4808;&#4845;</trd>
  <trd lng="ar">&a_y;&a_A;</trd>
  <trd lng="az">oh</trd>
  <trd lng="bn">&#2441;&#2489;&#2497;</trd>
  <trd lng="bs">oh</trd>
  <trd lng="co">oh</trd>
  <trdgrp lng="cs">
    <trd>jak&eacute; ne&scaron;t&ecaron;st&iacute;</trd>,
    <trd>jej</trd>
  </trdgrp>
  <trd lng="da">oh</trd>
  <trdgrp lng="de">
     <trd>o</trd>,
     <trd>oh</trd>
