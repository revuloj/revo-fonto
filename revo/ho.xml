  <trd lng="lo">&#3778;&#3757;&#3785;&#3725;</trd>
  <trd lng="mg">&ocirc;</trd>
  <trd lng="mk">&c_o;&c_h;</trd>
  <trd lng="ml">&#3347;</trd>
  <trd lng="mn">&#1257;&#1257;</trd>
  <trd lng="mr">&#2348;&#2366;&#2346;&#2352;&#2375;</trd>
  <trd lng="ne">&#2361;&#2352;&#2375;</trd>
