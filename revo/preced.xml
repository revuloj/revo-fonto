  </trdgrp>
  <trd lng="pl">bezprecedensowy</trd>
  <trd lng="sk">bezprecedentn&yacute;</trd>
  <trd lng="uk">&#x431;&#x435;&#x437;&#x43F;&#x440;&#x435;&#x446;&#x435;&#x434;&#x435;&#x43D;&#x442;&#x43D;&#x438;&#x439;</trd>
  <trdgrp lng="zh">
