<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;erc</rad>/i
</kap>
<drv mrk="sxerc.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Diri a&ubreve; fari ion celante amuzi kaj ridigi.
    </dif>
  </snc>
  <snc>
    <dif>
      Neserioze paroli:
      <ekz>
        <ind><tld/>e</ind> dirite, cele pensite
        <fnt><bib>PrV</bib></fnt>.
        <trdgrp lng="be">
          <trd>&c_n;&c_a;&c_r;&c_o;&c_k;&c_a;&c_m;</trd>, 
          <trd>&c_sh;&c_u;&c_t;&c_k;&c_u;&c_ju;&c_ch;&c_y;</trd>, 
          <trd>&c_zh;&c_a;&c_r;&c_t;&c_u;&c_ju;&c_ch;&c_y;</trd>
        </trdgrp>
        <trd lng="ru">&c_v; &c_sh;&c_u;&c_t;&c_k;&c_u;</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_u;&c_t;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>, 
    <trd>&c_zh;&c_a;&c_r;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>    
  </trdgrp>
  <trd lng="de">scherzen</trd>
  <trd lng="en">joke</trd>
  <trdgrp lng="he">
    <trd>&lamed;&he;&tav;<ind>&bet;&dalet;&het;</ind></trd>,
    <trd>&lamed;&he;&tav;&lamed;&vav;&tsadi;&ftsadi;
\<ind>&lamed;&tsadi;&ftsadi;</ind>\</trd>
  </trdgrp>
  <trd lng="hu">tr&eacute;f&aacute;l</trd>
  <trdgrp lng="nl">
    <trd>grappig zijn</trd>,
    <trd>schertsen</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>gracejar</trd>,
    <trd>brincar</trd>,
    <trd>ca&ccedil;oar</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_u;&c_t;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sv">sk&auml;mta</trd>
</drv>
</art>
