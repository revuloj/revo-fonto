<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>cian</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="cian.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc mrk="cian.0o.RAD">
    <dif>
      CN. Radikalo formita de karbono kaj nitrogeno.
    </dif>
    <trd lng="be">&c_c;&c_y;&c_ja;&c_n; <klr tip="amb">(&c_r;&c_a;&c_d;&c_y;&c_k;&c_a;&c_l;)</klr></trd>
    <trd lng="de">Cyangruppe</trd>
    <trd lng="en">cyanogen</trd>
    <trdgrp lng="hu">
      <trd>ci&aacute;n</trd>,
      <trd>ci&aacute;ncsoport</trd></trdgrp>
    <trd lng="nl">cyaangroep</trd>
    <trd lng="ru">&c_c;&c_i;&c_a;&c_n;<klr tip="amb"> (&c_r;&c_a;&c_d;&c_i;&c_k;&c_a;&c_l;)</klr></trd>
  </snc>
  <snc mrk="cian.0o.GAS">
    <dif>
      (CN)<sub>2</sub>. Diciano. Senkolora, tre venena gaso, formita de
      karbono kaj nitrogeno.
    </dif>
    <adm>
      Venu ankora&ubreve; aparta artikolo pri diciano, kun ref al Kemia kaj
      Scienc-teknika vortaro <aut>PP</aut>
    </adm>
    <trdgrp lng="be">
      <trd>&c_c;&c_y;&c_ja;&c_n; <klr tip="amb">(&c_g;&c_a;&c_z;)</klr></trd>,
      <trd>&c_d;&c_y;&c_c;&c_y;&c_ja;&c_n;</trd>
    </trdgrp>
    <trd lng="en">cyanogen</trd>
    <trd lng="fr">cyanog&egrave;ne</trd>
    <trd lng="de">Dicyan</trd>
    <trd lng="hu">dici&aacute;n</trd>
    <trd lng="it">cianogeno</trd>
    <trd lng="nl">diciaan</trd>
    <trdgrp lng="ru">
      <trd>&c_c;&c_i;&c_a;&c_n;</trd>,
      <trd>&c_d;&c_i;&c_c;&c_i;&c_a;&c_n;</trd>
    </trdgrp>
  </snc>
</drv>
</art>
