<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>cian</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="cian.0o.RAD">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      CN. Radikalo formita de karbono kaj
      nitrogeno.
    </dif>
  </snc>
  <trd lng="de">Cyangruppe</trd>
  <trd lng="en">cyanogen</trd>
  <trd lng="nl">cyaangroep</trd>
  <trd lng="ru">&c_c;&c_i;&c_a;&c_n;</trd>
</drv>

<drv mrk="cian.0o.GAS">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      (CN)<sub>2</sub>. Diciano. Senkolora, tre venena gaso, formita de
      karbono kaj
      nitrogeno.
    </dif>
    <adm>
      Venu ankora&ubreve; aparta artikolo pri diciano, kun ref al Kemia kaj
      Scienc-teknika vortaro <aut>PP</aut>
    </adm>
  </snc>
  <trd lng="en">cyanogen</trd>
  <trd lng="fr">cyanog&egrave;ne</trd>
  <trd lng="de">Dicyan</trd>
  <trd lng="nl">diciaan</trd>
</drv>
</art>
