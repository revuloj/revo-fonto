<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cian.xml,v 1.19 2023/10/31 13:37:03 revo Exp $">
<kap>
  <rad>cian</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="cian.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc mrk="cian.0o.RAD">
    <dif>
      CN. <ref tip="super" cel="radika1.0o.KEM">Radikalo</ref> formita de karbono kaj nitrogeno:
      <ekz>
        elfluinta <tld/>o <klr>(cianido)</klr> murdis la riveron ne indulgante anka&ubreve; la
        hungaran terenon
        <fnt>
          <aut>Bela Apati Kovacs</aut>
          <vrk><url ref="http://www.eventoj.hu/arkivo/eve-183.htm">Morto de la naturo, el Vesperto n-ro 083</url></vrk>
          <lok>Eventoj n-ro 183</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_c;&c_y;&c_ja;&c_n; <klr tip="amb">(&c_r;&c_a;&c_d;&c_y;&c_k;&c_a;&c_l;)</klr></trd>
    <trd lng="de">Cyangruppe</trd>
    <trd lng="en">cyanogen</trd>
    <trdgrp lng="hu">
      <trd>ci&aacute;n</trd>,
      <trd>ci&aacute;ncsoport</trd></trdgrp>
    <trd lng="nl">cyaangroep</trd>
    <trd lng="pl">cyjan</trd>
    <trd lng="ru">&c_c;&c_i;&c_a;&c_n;<klr tip="amb">
    (&c_r;&c_a;&c_d;&c_i;&c_k;&c_a;&c_l;)</klr></trd>
  </snc>
  <snc mrk="cian.0o.GAS">
    <dif>
      (CN)<sub>2</sub>. Diciano; senkolora, tre venena gaso, formita de
      karbono kaj nitrogeno.
    </dif>
    <adm>
      Ni kreu ankora&ubreve; apartan artikolon pri diciano, kun ref al Kemia
      kaj Scienc-teknika vortaro <aut>PP</aut>
    </adm>
    <trdgrp lng="be">
      <trd>&c_c;&c_y;&c_ja;&c_n; <klr tip="amb">(&c_g;&c_a;&c_z;)</klr></trd>,
      <trd>&c_d;&c_y;&c_c;&c_y;&c_ja;&c_n;</trd>
    </trdgrp>
    <trd lng="en">cyanogen</trd>
    <trd lng="fr">cyanog&egrave;ne</trd>
    <trd lng="de">Dicyan</trd>
    <trd lng="hu">dici&aacute;n</trd>
    <trd lng="it">cianogeno</trd>
    <trd lng="nl">diciaan</trd>
    <trd lng="pl">cyjan</trd>
    <trdgrp lng="ru">
      <trd>&c_c;&c_i;&c_a;&c_n;</trd>,
      <trd>&c_d;&c_i;&c_c;&c_i;&c_a;&c_n;</trd>
    </trdgrp>
    <trd lng="sv">dicyan</trd>
    <trd lng="zh">&#x6C30; <pr>q&#xED;ng</pr></trd>
</snc>
  <trd lng="cs">kyan <klr>(radik&aacute;l)</klr></trd>
  <trd lng="ja">&#x30B7;&#x30A2;&#x30F3;</trd>
  <trd lng="sk">nev&auml;dza</trd>
  <trd lng="uk">&c_c;&c_ib;&c_a;&c_n;</trd>
</drv>
</art>
