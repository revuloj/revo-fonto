  <trd lng="hu">kort&aacute;rs</trd>
  <trd lng="nl">tijdgenoot</trd>
  <trd lng="pl">wsp&oacute;&lstroke;czesny <klr>(komu)</klr></trd>
  <trd
lng="ru">&c_s;&c_o;&c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_i;&c_k;</trd>
</drv>

<drv mrk="temp.sam0e.kun">
  <kap>sam<tld/>e kun<fnt>Z</fnt></kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc>
    <dif>
      En la sama momento, tempero a&ubreve; epoko kiel:
      <ekz>
        ili ekaperis sam<tld/>e kun la stalinaj soldatoj
        <fnt>
        <aut>Volodimir Hurtovenko</aut>,
        <vrk>Kial la rusa ortodoksa eklezio malamas la papon?</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2002a, numero 2a, p. 15a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_n;&c_a;&c_ch;&c_a;&c_s;&c_n;&c_a;</trd>,
    <trd>&c_a;&c_d;&c_n;&c_a;&c_ch;&c_a;&c_s;&c_o;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="fr">en m&ecirc;me temps que</trd>
  <trdgrp lng="hu">
    <trd>egy <ind>id&odblac;ben</ind></trd>,
    <trd>egyidej&udblac;leg</trd>
  </trdgrp>
  <trd lng="nl">tegelijker tijd</trd>
  <trdgrp lng="pl">
    <trd>jednocze&sacute;nie</trd>,
    <trd>r&oacute;wnocze&sacute;nie</trd>,
  </trdgrp>
  <trd
lng="ru">&c_o;&c_d;&c_n;&c_o;&c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_o;&c_s;</trd>
</drv>

<drv mrk="temp.sam0ulo">
