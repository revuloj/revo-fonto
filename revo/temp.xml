<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>temp</rad>/o
    </kap>

    <drv mrk="temp.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="temp.0o.cxiamo">
	<dif>
	  Senlima da&ubreve;ro konsiderata kiel universala interrilato
	  la&ubreve; kiu ordi&gcirc;as la sinsekvaj fenomenoj:
	  <ekz>
	    <tld/>o estas mono<fnt>Z</fnt>;
	  </ekz><ekz>
	    konsolon <tld/>o donas al la koro<fnt>Z</fnt>;
	  </ekz><ekz>
	    <tld/>o toleras, sed vero aperas
	    <fnt><bib>PrV</bib></fnt>.
	  </ekz>
	</dif>
	<trd lng="de">Zeit</trd>
	<trd lng="ru">&c_v;&c_r;&c_je;&c_m;&c_ja;</trd>
      </snc>
      <snc mrk="temp.0o.FIZ">
	<uzo tip="fak">FIZ</uzo>
	<dif>
	  Baza <ref tip="super" cel="grand.fizika0o">fizika grando</ref>,
	  esprimanta la da&ubreve;ron de fenomeno a&ubreve; la momenton, en kiu
	  &gcirc;i okazas:
	  <ekz>
	    la <ref cel="dimens.0o.FIZ">dimensioj</ref> de <tld/>o estas
	    <frm>M<sup>0</sup>L<sup>0</sup>T<sup>1</sup></frm>.
	  </ekz>
	</dif>
	<refgrp tip="vid">
	  <ref cel="sekund.0o.tempo">sekundo</ref>.
	</refgrp>
	<trd lng="fr">temps<klr tip="ind"> (grandeur physique)</klr></trd>
	<trd lng="ru">&c_v;&c_r;&c_je;&c_m;&c_ja;</trd>
      </snc>
      <snc mrk="temp.0o.tempero">
	<dif>
	  Ia aparta, pli malpli longa, parto de la da&ubreve;ro:
	  <ekz>
	    alia <tld/>o, aliaj moroj
	    <fnt><bib>PrV</bib></fnt>;
	  </ekz><ekz>
	    en la bona malnova <tld/>o
	    <fnt><bib>PrV</bib></fnt>;
	  </ekz><ekz>
	    la <tld/>o pasinta neniam revenos<fnt>Z</fnt>;
	  </ekz><ekz>
	    li mortis anta&ubreve; nelonga <tld/>o<fnt>Z</fnt>;
	  </ekz><ekz>
	    en <tld/>o de malvarmo<fnt>Z</fnt>;
	  </ekz><ekz>
	    ne konvenas pluvo en <tld/>o de rikolto<fnt>Z</fnt>;
	  </ekz><ekz>
	    oni povas en la unua <tld/>o tute ne uzi la artikolon<fnt>Z</fnt>;
	  </ekz><ekz>
	    en &ccirc;iu <tld/>o amiko amas<fnt>Z</fnt>.
	  </ekz>
	</dif>
	<refgrp tip="vid">
	  <ref cel="epok.0o">epoko</ref>,
	  <ref cel="moment.0o">momento</ref>,
	  <ref cel="period.0o">periodo</ref>,
	  <ref cel="sezon.0o">sezono</ref>
	</refgrp>
	<trd lng="ru">&c_v;&c_r;&c_je;&c_m;&c_ja;</trd>
      </snc>
      <snc mrk="temp.0o.tauxga">
	<dif>
	  Difinita, konvena, ta&ubreve;ga <tld/>o por io:
	  <ekz>
	    bona estas vorto en la &gcirc;usta <tld/>o<fnt>Z</fnt>;
	  </ekz><ekz>
	    jam estas <tld/>o iri domen<fnt>Z</fnt>;
	  </ekz><ekz>
	    estas <tld/>o por plori kaj <tld/>o por ridi<fnt>Z</fnt>;
	  </ekz><ekz>
	    ne estas nun <tld/>o por amuzi&gcirc;i;
	  </ekz><ekz>
	    la <tld/>o de la ven&gcirc;o<fnt>Z</fnt>;
	  </ekz><ekz>
	    <ind>&gcirc;usta<tld/>e</ind>
	    kiel pluvo en a&ubreve;gusto, kiel froma&gcirc;o sur makaronioj
	    <trd lng="ru">&c_s;&c_v;&c_o;&c_je;&c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_o;</trd>
	  </ekz>
	</dif>
	<refgrp tip="vid">
	  <ref cel="vok.0i">voki</ref>.
	</refgrp>
	<trdgrp lng="ru">
	  <trd>&c_v;&c_r;&c_je;&c_m;&c_ja;</trd>,
	  <trd>&c_s;&c_r;&c_o;&c_k;</trd>
	</trdgrp>
      </snc>
      <snc mrk="temp.0o.libera">
	<dif>
	  Libere disponebla <tld/>o:
	  <ekz>
	    tio ne okupos <klr>(forprenos)</klr> multe da <tld/>o<fnt>Z</fnt>;
	  </ekz><ekz>
	    mi ne havas <tld/>on;
	  </ekz><ekz>
	    ne perdu vian <tld/>on
	  </ekz>
	</dif>
	<refgrp tip="vid">
	  <ref cel="divid.0i">dividi</ref>
	</refgrp>
	<trd lng="ru">&c_v;&c_r;&c_je;&c_m;&c_ja;</trd>
      </snc>
      <snc mrk="temp.0o.GRA">
	<uzo tip="fak">GRA</uzo>
	<dif>
	  Speciala fleksio de verbo montranta, ke la priparolata
	  ago estas nuna (prezenco), pasinta (preterito)
	  a&ubreve; estonta (futuro).
	</dif>
	<refgrp tip="vid">
	  <ref cel="modal.0o.GRA.modo">modalo</ref>.
	</refgrp>
	<trd lng="ru">&c_v;&c_r;&c_je;&c_m;&c_ja;</trd>
      </snc>
      <snc mrk="temp.0o.MUZ">
	<uzo tip="fak">MUZ</uzo>
	<uzo tip="stl">EVI</uzo>
	<ref tip="dif" cel="puls.0o.MUZ">Pulso</ref>
	<trd lng="ru">&c_d;&c_o;&c_l;&c_ja; <klr>(&c_m;&c_u;&c_z;., &c_u;&c_s;&c_t;.)</klr></trd>
      </snc>
      <snc mrk="temp.0o.MIT">
	<uzo tip="fak">MIT</uzo>
	<dif>
	  Mita dia&jcirc;o, personigo de la senlima da&ubreve;ro sub formo
	  de maljunulo kun longa barbo tenanta kiel atributojn
	  fal&ccirc;ilon kaj sablhorlo&gcirc;on.
	</dif>
	<adm>
	  Necesas apartigi &ccirc;i tiun sencon por &gcirc;in majuskligi.
	  <aut>MB</aut>
	</adm>
	<trd lng="ru">&c_V;&c_r;&c_je;&c_m;&c_ja;</trd>
      </snc>
      <trd lng="cs">&ccaron;as</trd>
      <trd lng="en">time</trd>
      <trd lng="es">tiempo</trd>
      <trd lng="fr">temps<klr tip="ind"> (t.s. sauf m&eacute;t&eacute;o)</klr></trd>
      <trd lng="he">&zayin;&mem;&fnun;</trd>
      <trd lng="hu">id&odblac;</trd>
      <trd lng="nl">tijd</trd>
      <trdgrp lng="tr">
	<trd>zaman</trd>,
	<trd>vakit</trd>
      </trdgrp>
      <trd lng="sk">&ccaron;as</trd>
      <trd lng="vo">tim</trd>
      <trdgrp lng="zh">
	<trd>&#26102;</trd>,
	<trd><klr>tr. </klr>&#26178;<klr> [sh&iacute;]</klr></trd>,
	<trd>&#26102;&#38388;</trd>,
	<trd><klr>tr. </klr>&#26178;&#38291;<klr> [sh&iacute;ji&#257;n]</klr></trd>,
	<trd>&#26102;&#20505;</trd>,
	<trd><klr>tr. </klr>&#26178;&#20505;<klr> [sh&iacute;hou]</klr></trd>
      </trdgrp>
    </drv>

    <drv mrk="temp.antaux0a">
      <kap>anta&ubreve;<tld/>a</kap>
      <snc>
	<dif>
	  Okazanta anta&ubreve; la ta&ubreve;ga <tld/>o; trofrua:
	  <ekz>
	    anta&ubreve;<tld/>a morto, aku&scirc;o<fnt>B</fnt>.
	  </ekz>
	</dif>
      </snc>
      <trd lng="fr">pr&eacute;matur&eacute;</trd>
      <trd lng="de">vorzeitig</trd>
      <trdgrp lng="hu">
	<trd>id&odblac; el&odblac;tti</trd>,
	<trd>korai</trd>
      </trdgrp>
      <trd lng="ru">&c_p;&c_r;&c_je;&c_zh;&c_d;&c_je;&c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
    </drv>

    <drv mrk="temp.de0e.de">
      <kap>de <tld/>e de</kap>
      <gra><vspec>prepozicia&jcirc;o</vspec></gra>
      <snc>
	<dif>
	  Ek<ref tip="super" cel="de.0.prep_tempo">de</ref> la epoko de:
	  <ekz>
	    li estas batalisto de<tld/>e de siaj plej junaj jaroj<fnt>Z</fnt>;
	  </ekz><ekz>
	    <ind>de<tld/>e de</ind> la fondado de la tero<fnt>Z</fnt>.
	  </ekz>
	</dif>
      </snc>
      <trdgrp lng="fr">
	<trd>depuis</trd>
      </trdgrp>
      <trd lng="ru">&c_s;&c_o; &c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_i;</trd>
    </drv>

    <drv mrk="temp.de0o.al.0o">
      <kap>de <tld/>o al <tld/>o</kap>
      <snc>
	<dif>
	  Iam kaj iam, plurfoje la&ubreve; inter<tld/>oj, <ref tip="vid"
	    cel="interm.0e">intermite</ref>:
	  <ekz>
	    de <tld/>o al <tld/>o la flamo ekflagradis<fnt>Z</fnt>.
	  </ekz>
	</dif>
      </snc>
      <trdgrp lng="fr">
	<trd>de <ind>temps</ind> &agrave; autre</trd>,
	<trd>de <ind>temps</ind> en temps</trd>
      </trdgrp>
      <trd lng="ru">&c_v;&c_r;&c_je;&c_m;&c_ja; &c_o;&c_t; &c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_i;</trd>
    </drv>

    <drv mrk="temp.for0igxi">
      <kap>for<tld/>i&gcirc;i</kap>
      <snc>
	<dif>
	  Arkaiki&gcirc;i, malaktuali&gcirc;i; perdi&gcirc;i, senvalori&gcirc;i
	  pro troa prokrasto.
	</dif>
	<refgrp tip="sin">
	  <ref cel="kaduk.0igxi">kaduki&gcirc;i</ref>,
	  <ref cel="valid">senvalidi&gcirc;i</ref>.
	</refgrp>
      </snc>
      <trd lng="fr">avoir fait son <ind>temps</ind></trd>
      <trd lng="hu">el&eacute;v&uuml;l</trd>
      <trd lng="ru">&c_u;&c_s;&c_t;&c_a;&c_r;&c_je;&c_t;&c_mol;</trd>
    </drv>

    <drv mrk="temp.liber0o">
      <kap>liber<tld/>o</kap>
      <snc>
	<dif>
	  <tld lit="T"/>o, dum kiu oni ne estas devigata labori
	  (precipe pri lernantoj a&ubreve; oficistoj).
	</dif>
	<refgrp tip="vid">
	  <ref cel="feri.0o">ferio</ref>
	</refgrp>
      </snc>
      <trd lng="de">Freizeit</trd>
      <trd lng="fr">temps libre</trd>
      <trdgrp lng="he">
	<trd>&het;&vav;&pe;&shin;&he;<klr> (&mem;&ayin;&bet;&vav;&dalet;&he; &alef;&vav;
	    &mem;&lamed;&yod;&mem;&vav;&dalet;&yod;&fmem;)</klr></trd>,
	<trd>&pe;&gimel;&resh;&he;</trd>
      </trdgrp>
      <trd lng="hu">szabadid&odblac;</trd>
      <trd lng="nl">vrijetijd</trd>
      <trd lng="ru">&c_s;&c_v;&c_o;&c_b;&c_o;&c_d;&c_n;&c_o;&c_je; &c_v;&c_r;&c_je;&c_m;&c_ja;</trd>
    </drv>

    <drv mrk="temp.prov0o">
      <kap>prov(o)<tld/>o</kap>
      <snc>
	<dif>
	  Periodo, dum kiu okazas <ref cel="prov.0o">provo</ref>:
	  <ekz>
	    &ccirc;u tio ne pruvas, ke liaj suferoj estis nur Dia provo<tld/>o?
	    <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 1</lok></fnt>;
	  </ekz><ekz>
	    post certa provo<tld/>o ni decidi&gcirc;is al formo de
	    &leftquot;Lexicon Silvestre&rightquot;, kiu ampleksu
	    la informeblecojn de &ccirc;iuj kvar vortaroformoj
	    <fnt>
	      <vrk>Pri la internacia lingva interkompreni&gcirc;o kadre de la forsta fako</vrk>
	      <lok><url ref="http://esperantic.org/librejo/dbstudoj/41_Simon.htm">http://esperantic.org/librejo/dbstudoj/41_Simon.htm</url></lok></fnt>;
	  </ekz><ekz>
	    ekde 1-a de januaro 1989 estis redonita al li por unujara prov<tld/>o
	    &scirc;tata permeso por praktikado de animzorga servo
	    <fnt>
	      <vrk>Dio benu</vrk>
	      <lok><url ref="http://home.t-online.de/home/b.eichkorn/dibe46.htm">http://home.t-online.de/home/b.eichkorn/dibe46.htm</url></lok></fnt>;
	  </ekz><ekz>
	    estis kondamnita la tendarestro Milo&scirc; je 15 monatoj kaj
	    pastro Srna je 12 monatoj de karcera restado kun trijara
	    prokrasta prov<tld/>o
	    <fnt><vrk>Dio benu</vrk><lok><url ref="http://mujweb.atlas.cz/spolecnost/svacek/41DIOBEN.HTM">http://mujweb.atlas.cz/spolecnost/svacek/41DIOBEN.HTM</url></lok></fnt>;
	  </ekz><ekz>
	    oni promesis al ni sesmonatan da&ubreve;ron kiel prov<tld/>on
	    <fnt><vrk>Esperanto sub la Suda Kruco</vrk><lok><url ref="http://www.sudakruco.org/012/09.html">1994-11/12</url></lok></fnt>.
	  </ekz>
	</dif>
      </snc>
      <trdgrp lng="fr">
	<trd>p&eacute;riode d'essai</trd>,
	<trd>p&eacute;riode probatoire</trd>,
	<trd>probation</trd>
      </trdgrp>
      <trd lng="ru">&c_i;&c_s;&c_p;&c_y;&c_t;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_s;&c_r;&c_o;&c_k;</trd>
    </drv>

    <drv mrk="temp.real0a">
      <kap>real<tld/>a</kap>
      <snc mrk="temp.real0a.KOMP">
	<uzo tip="fak">KOMP</uzo>
	<dif>
	  Rilata al tia funkciado de
	  <ref cel="komput.">komuta sistemo</ref>, ke
	  &gcirc;iaj interagon kun eksteraj procezoj okazas sufi&ccirc;e
	  rapide por la&ubreve;cele influi tiujn procezojn:
	  <ekz>
	    <ind>real<tld/>a re&gcirc;imo</ind>;
	    <trd lng="en">real-time operation</trd>
	    <trd lng="ru">&c_r;&c_je;&c_zh;&c_i;&c_m; <ind>&c_r;&c_je;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_g;&c_o;
		&c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_i;</ind></trd>
	  </ekz><ekz>
	    real<tld/>a komputila tradukado de telefonaj
	    konversacioj
	    <fnt>
	      <aut>R. F&ouml;ssmeier</aut>:
	      <lok><url
		  ref="http://www.forst.tu-muenchen.de/EXT/AIS/kursoj/kultprog/teksto.html"
		  >Kulturaj aspektoj de komputil-programado</url></lok>.
	    </fnt>.
	  </ekz>
	</dif>
      </snc>
      <trd lng="fr">temps r&eacute;el</trd>
      <trd lng="he">&zayin;&mem;&fnun;-&alef;&mem;&tav;</trd>
      <trd lng="hu">val&odblac;s idej&udblac;</trd>
    </drv>

    <drv mrk="temp.sam0a">
      <kap><tld/>a</kap>
      <snc>
	<dif>
	  <ref tip="vid" cel="kun.0a">Kuna</ref> la&ubreve; <tld/>o, okazanta en la
	  sama momento a&ubreve; epoko.
	</dif>
      </snc>
      <trdgrp lng="fr">
	<trd>contemporain<klr> (adj.)</klr></trd>,
	<trd>simultan&eacute;</trd>,
	<trd>synchrone</trd>
      </trdgrp>
      <trd lng="ru">&c_o;&c_d;&c_n;&c_o;&c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
    </drv>

    <drv mrk="temp.sam0ano">
      <kap>sam<tld/>ano</kap>
      <snc>
	<dif>
	  Homo a&ubreve; afero kies ekzistoperiodo havas komunan parton
	  kun la ekzistoperiodo de iu a&ubreve; io alia:
	  <ekz>
	    genia judo nomata Eleazer ben Judah, sam<tld/>ano kaj
	    kuracista kolego de Zamenhof
	    <fnt>
	      <aut>Ismael Gomes BRAGA</aut>:
	      <vrk>Sperto de Zamenhof</vrk>.
	      <lok>Nica literatura revuo, 1:1</lok>
	    </fnt>;
	  </ekz><ekz>
	    sam<tld/>ano de la grandaj malkovroj geografiaj kaj de la
	    Inkvizicio.
	  </ekz>
	</dif>
      </snc>
      <trd lng="fr">contemporain<klr> (subst.)</klr></trd>
      <trd lng="ru">&c_s;&c_o;&c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_i;&c_k;</trd>
    </drv>

    <drv mrk="temp.sam0e.kun">
      <kap>sam<tld/>e kun<fnt>Z</fnt></kap> 
      <gra><vspec>prepozicia&jcirc;o</vspec></gra>
      <snc>
	<dif>
	  En la sama momento, tempero a&ubreve; epoko kiel:
	  <ekz>
	    Ili ekaperis sam<tld/>e kun la stalinaj soldatoj
	    <fnt><aut>Volodimir Hurtovenko</aut>: <vrk>Kial la rusa ortodoksa
		eklezio malamas la papon?</vrk>, <bib>Monato</bib>, <lok>jaro
		2002a, numero 2a, p. 15a</lok></fnt>.
	  </ekz>
	</dif>
      </snc>
      <trd lng="fr">en m&ecirc;me temps que</trd>
      <trdgrp lng="hu">
	<trd>egy <ind>id&odblac;ben</ind></trd>,
	<trd>egyidej&udblac;leg</trd>
      </trdgrp>
      <trd lng="ru">&c_o;&c_d;&c_n;&c_o;&c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_o; &c_s;</trd>
    </drv>

    <drv mrk="temp.sam0ulo">
      <kap>sam<tld/>ulo</kap>
      <snc>
	<dif>
	  Homo kiu estas <ref tip="sub" cel="temp.sam0ano">sam<tld/>ano</ref>.
	</dif>
      </snc>
      <trd lng="fr">contemporain<klr> (subst.)</klr></trd>
      <trd lng="ru">&c_s;&c_o;&c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_i;&c_k;</trd>
    </drv>

    <drv mrk="temp.sia0e">
      <kap>sia<tld/>e</kap>
      <snc>
	<dif>
	  Respektive en sia epoko, konsiderante la tiamajn kondi&ccirc;ojn:
	  <ekz>
	    La verkinto preferas uzi la vorton &leftquot;ka&ccirc;aloto&rightquot;, <klr>...</klr>
	    anstata&ubreve; la vorto &leftquot;makrocefalo&rightquot;, kiun Zamenhof siatempe iam uzis
	    <fnt><aut>Wim M. A. De Smet</aut>: <vrk>Gigantoj sur
		strandoj</vrk>, <bib>Monato</bib>, <lok>jaro 1998a, numero 11a, p.
		22a</lok></fnt>.
	  </ekz>
	</dif>
      </snc>
      <trdgrp lng="fr">
	<trd>en son <ind>temps</ind></trd>
      </trdgrp>
      <trdgrp lng="ru">
	<trd>&c_v; &c_s;&c_v;&c_o;&c_jo; <ind>&c_v;&c_r;&c_je;&c_m;&c_ja;</ind></trd>
      </trdgrp>
    </drv>

    <drv mrk="temp.somera0o">
      <kap>somera <tld/>o</kap>
      <snc mrk="temp.somera0o.somero">
	<dif>
	  <tld lit="T"/>operiodo en somero:
	  <ekz>
	    la betulo movi&gcirc;adis en la vento, estis en &gcirc;i vivo, kiel en
	    la arboj en somera <tld/>o
	    <fnt><bib>Fab4</bib>, <lok>Ne&gcirc;ulo</lok></fnt>.
	  </ekz>
	</dif>
      </snc>
      <snc mrk="temp.somera0o.horkalkulo">
	<dif>
	  <tld lit="T"/>o dum kiu la horlo&gcirc;oj estas fruigitaj
	  (anta&ubreve;enigitak) je 1 horo (a&ubreve; pli ol 1 horo) kompare kun la
	  normala tempokalkulo, por pligrandigi la uzeblon de la
	  taglumo fine de la labortagoj; la horkalkulo uzata dum tiu
	  periodo:
	  <ekz>
	    ni atentigu, ke la&ubreve; la somera <tld/>o niaj programoj
	    a&ubreve;di&gcirc;os je unu horo pli frue
	    <fnt><lok><url
	    ref="http://www.bavaresperantoinfo.de/0203/aktuell1.htm"
