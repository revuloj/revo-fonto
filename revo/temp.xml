<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>temp</rad>/o</kap>
<drv mrk="temp.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="temp.0o.cxiamo">
    <dif>
      Senlima da&ubreve;ro konsiderata kiel universala interrilato,
      la&ubreve; kiu ordi&gcirc;as la sinsekvaj fenomenoj:
      <ekz>
        <tld/>o estas mono<fnt>Z</fnt>;
      </ekz>
      <ekz>
        konsolon <tld/>o donas al la koro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>o toleras, sed vero aperas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Zeit</trd>
    <trd lng="ru">&c_v;&c_r;&c_je;&c_m;&c_ja;</trd>
  </snc>
  <snc mrk="temp.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Baza <ref tip="super" cel="grand.fizika0o">fizika grando</ref>,
      esprimanta la da&ubreve;ron de fenomeno a&ubreve; la momenton, en kiu
      &gcirc;i okazas:
      <ekz>
        la <ref cel="dimens.0o.FIZ">dimensioj</ref> de <tld/>o estas
        <frm>M<sup>0</sup>L<sup>0</sup>T<sup>1</sup></frm>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sekund.0o.tempo">sekundo</ref>
    </refgrp>
    <trd lng="de">Zeit</trd>
    <trd lng="fr">temps<klr tip="ind"> (grandeur physique)</klr></trd>
    <trd lng="ca">temps</trd>
    <trd lng="es">tiempo</trd>
    <trd lng="pl">czas</trd>
  <trdgrp lng="pt">
   <trd>tempo</trd>
  </trdgrp>
    <trd lng="ru">&c_v;&c_r;&c_je;&c_m;&c_ja;</trd>
  </snc>
  <snc mrk="temp.0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Ia aparta, pli malpli longa, parto de la da&ubreve;ro:
      <ekz>
        la <tld/>o pasinta jam neniam revenos
        <fnt><bib>F</bib>, <lok>&FE; 22</lok></fnt>;
      </ekz>
      <ekz>
        la pastro, kiu mortis <klr>...</klr> anta&ubreve; nelonga <tld/>o
        <fnt><bib>F</bib>, <lok>&FE; 25</lok></fnt>;
      </ekz>
      <ekz>
        <ind>alia <tld/>o, aliaj moroj</ind>
        <trd lng="fr">autres temps, autres m&oelig;urs</trd>
        <trd lng="hu">m&aacute;s <ind>id&odblac;k</ind>, m&aacute;s
erk&ouml;lcs&ouml;k</trd>       
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>en la bona malnova <tld/>o</ind>
        <trd lng="fr">au bon vieux temps</trd>
        <trd lng="hu">a r&eacute;gi sz&eacute;p id&odblac;kben</trd>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        en <tld/>o de malvarmo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ne konvenas pluvo en <tld/>o de rikolto<fnt>Z</fnt>;
      </ekz>
      <ekz>
        personoj <klr>...</klr> povas en la unua <tld/>o tute ne uzi la
        artikolon
        <fnt><bib>F</bib>, <lok>&FE; 27</lok></fnt>;
      </ekz>
      <ekz>
        en &ccirc;iu <tld/>o amiko amas<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="epok.0o">epoko</ref>,
      <ref cel="moment.0o">momento</ref>,
      <ref cel="period.0o">periodo</ref>,
      <ref cel="sezon.0o">sezono</ref>
    </refgrp>
    <trd lng="de">Zeit</trd>
    <trd lng="ru">&c_v;&c_r;&c_je;&c_m;&c_ja;</trd>
    <trd lng="ca">temps</trd>
    <trd lng="es">tiempo</trd>
    <trd lng="pl">czas</trd>
  </snc>
  <snc mrk="temp.0o.tauxga">
    <dif>
      Difinita, konvena, libera <tld/>o por io:
      <ekz>
        jam estas <tld/>o iri domen<fnt>
        <bib>F</bib>, <lok>&FE; 28</lok></fnt>;
      </ekz>
      <ekz>
        bona estas vorto en la &gcirc;usta <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        estas <tld/>o por plori kaj <tld/>o por ridi<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ne estas nun <tld/>o por amuzi&gcirc;i;
      </ekz>
      <ekz>
        la <tld/>o de la ven&gcirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tio ne okupos <klr>(forprenos)</klr> multe da <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi ne havas <tld/>on;
      </ekz>
      <ekz>
        ne perdu vian <tld/>on.
      </ekz>
    </dif>
    <trd lng="de">Zeit</trd>
    <trd lng="pl">pora</trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_r;&c_je;&c_m;&c_ja;</trd>,
      <trd>&c_s;&c_r;&c_o;&c_k;</trd>
    </trdgrp>
  </snc>
  <snc mrk="temp.0o.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      Fleksio de verbo montranta, ke la priparolata
      ago estas nuna (prezenco), pasinta (preterito)
      a&ubreve; estonta (futuro).
    </dif>
    <refgrp tip="vid">
      <ref cel="modal.0o.GRA.modo">modalo <sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Zeit</trd>,
      <trd>Tempus</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_r;&c_je;&c_m;&c_ja;</trd>
  </snc>
  <snc mrk="temp.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <uzo tip="stl">EVI</uzo>
    <ref tip="dif" cel="puls.0o.MUZ">pulso</ref>
    <trd lng="be">&c_d;&c_o;&c_l;&c_ja;
<klr>(&c_m;&c_u;&c_z;&c_y;&c_ch;&c_n;&c_a;&c_ja;)</klr></trd>
    <trd lng="ru">&c_d;&c_o;&c_l;&c_ja; <klr>(&c_m;&c_u;&c_z;.,
&c_u;&c_s;&c_t;.)</klr></trd>
    <trdgrp lng="de">
      <trd>Zeit</trd>,
      <trd>Takt</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_s;</trd>,
    <trd>&c_t;&c_e;&c_r;&c_m;&c_ib;&c_n;</trd>
  </trdgrp>
  <trd lng="cs">&ccaron;as</trd>
  <trd lng="en">time</trd>
  <trd lng="es">tiempo</trd>
  <trd lng="fr">temps<klr tip="ind"> (t. s. sauf
m&eacute;t&eacute;o)</klr></trd>
  <trd lng="he">&zayin;&mem;&fnun;</trd>
  <trd lng="hu">id&odblac;</trd>
  <trd lng="nl">tijd</trd>
  <trd lng="pl">czas</trd>
  <trd lng="ro">timp</trd>
  <trd lng="sk">&ccaron;as</trd>
  <trdgrp lng="tr">
    <trd>zaman</trd>,
    <trd>vakit</trd>
  </trdgrp>
  <trd lng="vo">tim</trd>
  <trdgrp lng="zh">
    <trd>&#26102;</trd>,
    <trd><klr>tr. </klr>&#26178;<klr> [sh&iacute;]</klr></trd>,
    <trd>&#26102;&#38388;</trd>,
    <trd><klr>tr. </klr>&#26178;&#38291;<klr>
[sh&iacute;ji&#257;n]</klr></trd>,
    <trd>&#26102;&#20505;</trd>,
    <trd><klr>tr. </klr>&#26178;&#20505;<klr> [sh&iacute;hou]</klr></trd>
  </trdgrp>
</drv>

<drv mrk="temp.T0o">
  <kap><tld lit="T"/>o</kap>
  <snc mrk="temp.T0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Figuro de la senlima <tld/>o kiel maljunulo kun longa barbo tenanta
      sablohorlo&gcirc;on kaj mortigan fal&ccirc;ilon.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Zeit</trd>
  </trdgrp>
  <trd lng="hu">Id&odblac;</trd>
  <trd lng="pl">Czas</trd>
  <trd lng="ru">&c_V;&c_r;&c_je;&c_m;&c_ja;</trd>
</drv>

<drv mrk="temp.0i">
  <kap><tld/>i</kap>
  <snc mrk="temp.0i.KOMUNE">
    <gra><vspec>sensubjekte</vspec></gra>
    <dif>
      Esti la difinita, ta&ubreve;ga <tld/>o por io:
      <ekz>
        jam <tld/>is bruligi lampon
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 106a, volumo 3a, p.
        379a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>c'est l'<ind>heure</ind></trd>,
    <trd>c'est le <ind>moment</ind></trd>
  </trdgrp>
  <trd lng="hu">itt az <ind>ideje</ind></trd>
  <trd lng="pl"><klr>(ju&zdot;)</klr> <ind>czas</ind></trd>
</drv>

<drv mrk="temp.antaux0a">
  <kap>anta&ubreve;<tld/>a</kap>
  <snc>
    <dif>
      Okazanta anta&ubreve; la ta&ubreve;ga <tld/>o; trofrua:
      <ekz>
        anta&ubreve;<tld/>a morto, aku&scirc;o<fnt>B</fnt>;
      </ekz>
      <ekz>
        liaj sociaj idealoj estas iom neordinaraj kaj anta&ubreve;<tld/>aj
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 2a, antikva
foiro</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_w;&c_ch;&c_a;&c_s;&c_n;&c_y;</trd>
  <trd lng="fr">pr&eacute;matur&eacute;</trd>
  <trd lng="de">vorzeitig</trd>
  <trdgrp lng="hu">
    <trd>id&odblac; el&odblac;tti</trd>,
    <trd>korai</trd>
  </trdgrp>
  <trd lng="pl">przedwczesny</trd>
  <trd lng="ro">&icirc;nainte de timp</trd>
  <trd
lng="ru">&c_p;&c_r;&c_je;&c_zh;&c_d;&c_je;&c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="temp.de0ede">
  <kap>de<tld/>e de</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <ref tip="dif" cel="de.0.tempo">de <sncref/></ref>
    <ekz>
      respondis Josuo, filo de Nun, kaj servanto de Moseo
      de<tld/>e de lia juneco
      <fnt><bib>MT</bib>, <lok>&Nom; 11:28</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z; &c_ch;&c_a;&c_s;&c_u;</trd>,
    <trd>&c_a;&c_d; &c_ch;&c_a;&c_s;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>seit</trd>,
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&egrave;s</trd>,
    <trd>depuis</trd>
  </trdgrp>
  <trd lng="hu">idej&eacute;t&odblac;l fogva</trd>
  <trd lng="nl">sedert</trd>
  <trd lng="pl">od <ind>czasu</ind></trd>
  <trd lng="ru">&c_s;&c_o; &c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_i;</trd>
</drv>

<drv mrk="temp.de0o.al.0o">
  <kap>de <tld/>o al <tld/>o</kap>
  <snc>
    <dif>
      Iam kaj iam, plurfoje la&ubreve; inter<tld/>oj, <ref tip="vid"
      cel="interm.0e">intermite</ref>:
      <ekz>
        de <tld/>o al <tld/>o la flamo en &gcirc;i ekflagradis
        <fnt><bib>Fab2</bib>, <lok>malnova strata lanterno</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_s; &c_a;&c_d; &c_ch;&c_a;&c_s;&c_u;</trd>,
    <trd>&c_r;&c_a;&c_z;-&c_p;&c_o;&c_r;&c_a;&c_z;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>von <ind>Zeit zu Zeit</ind></trd>,
    <trd>bisweilen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>de <ind>temps</ind> &agrave; autre</trd>,
    <trd>de <ind>temps</ind> en temps</trd>
  </trdgrp>
  <trd lng="hu">id&odblac;nk&eacute;nt</trd>
  <trd lng="nl">van <ind>tijd</ind> tot tijd</trd>
  <trd lng="pl">od <ind>czasu</ind> do czasu</trd>
  <trd lng="ro">uneori</trd>
  <trd lng="ru">&c_v;&c_r;&c_je;&c_m;&c_ja; &c_o;&c_t;
&c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_i;</trd>
</drv>

<drv mrk="temp.fiks0o">
  <kap>fiks<tld/>o</kap>
  <snc mrk="temp.fiks0o.kiam">
    <dif>
      Perioda limtempo por iu okaza&jcirc;o a&ubreve; ago:
      <ekz>
        sinjoro, &ndash; diris Marta, &ndash; post du tagoj estos la
        fiks<tld/>o, en kiu mi devas enporti la luan pagon por la
lo&gcirc;ejo
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_o;&c_sh;&c_n;&c_ib;
<ind>&c_t;&c_e;&c_r;&c_m;&c_ib;&c_n;</ind></trd>
  <trd lng="fr">&eacute;ch&eacute;ance</trd>
  <trdgrp lng="hu">
    <trd>lej&aacute;rat</trd>,
    <trd>esed&eacute;kess&eacute;g</trd>
  </trdgrp>
  <trd lng="nl">vervaldag</trd>
  <trd lng="pl">termin</trd>
  <trd lng="ro">termen</trd>
</drv>

<drv mrk="temp.for0igxi">
  <kap>for<tld/>i&gcirc;i</kap>
  <snc mrk="temp.for0igxi.kiam">
    <dif>
      Arkaiki&gcirc;i, malaktuali&gcirc;i; perdi&gcirc;i, senvalori&gcirc;i
      pro troa prokrasto.
    </dif>
    <refgrp tip="vid">
      <ref cel="kaduk.0igxi">kaduki&gcirc;i</ref>,
      <ref cel="valid">senvalidi&gcirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_s;&c_t;&c_a;&c_r;&c_e;&c_c;&c_mol;</trd>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>d&eacute;pass&eacute;</ind><klr> (dans le
temps)</klr></trd>,
    <trd>avoir fait son <ind>temps</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>el&eacute;v&uuml;l</trd>,
    <trd>elavul</trd>
  </trdgrp>
  <trd lng="pl">przedawni&cacute; si&eogonek;</trd>
  <trd lng="ru">&c_u;&c_s;&c_t;&c_a;&c_r;&c_je;&c_t;&c_mol;</trd>
</drv>

<drv mrk="temp.gxusta0e">
  <kap>&gcirc;usta<tld/>e</kap>
  <snc mrk="temp.gxusta0e.kiam">
    <dif>
      En la oportuna, planita a&ubreve; konvena tempo:
      <ekz>
        &gcirc;usta<tld/>e kiel pluvo en a&ubreve;gusto, kiel
froma&gcirc;o sur makaronioj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_v;&c_o;&c_je;&c_ch;&c_a;&c_s;&c_o;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>rechtzeitig</trd>,
    <trd>zur rechten Zeit</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&agrave; temps</trd>,
    <trd>au bon moment</trd>,
    <trd>en temps et en heure</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>id&odblac;ben</trd>,
    <trd>j&oacute;kor</trd>,
    <trd>idejekor&aacute;n</trd>
  </trdgrp>
  <trd lng="nl">tijdig</trd>
  <trdgrp lng="pl">
    <trd>w <ind>por&eogonek;</ind></trd>,
    <trd>we w&lstroke;a&sacute;ciwym <ind>czasie</ind></trd>
  </trdgrp>
  <trd lng="ro">la <ind>timp</ind></trd>
  <trd
lng="ru">&c_s;&c_v;&c_o;&c_je;&c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_o;</trd>
</drv>

<drv mrk="temp.inter0e">
  <kap>inter<tld/>e</kap>
  <snc>
    <dif>
      Inter du okaza&jcirc;oj, inter tiam kaj nun:
      <ekz>
        la muroj e&ccirc; &scirc;ajnis fremdaj, nekonataj, &ccirc;ar
inter<tld/>e oni
        riparis fundamente
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="dum.0e.kiam">dume <sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_y;&c_m; &c_ch;&c_a;&c_s;&c_a;&c_m;</trd>
  <trdgrp lng="de">
    <trd>w&auml;hrenddessen</trd>,
    <trd>unterdessen</trd>,
    <trd>derweilen</trd>,
    <trd>derweil</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cependant<klr tip="ind"> (pendant ce temps)</klr></trd>,
    <trd>entretemps</trd>,
    <trd>pendant ce temps</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ek&ouml;zben</trd>,
    <trd>ezalatt</trd>,
    <trd>ak&ouml;zben</trd>,
    <trd>azalatt</trd>
  </trdgrp>
  <trd lng="nl">ondertussen</trd>
  <trdgrp lng="pl">
    <trd>tymczasem</trd>,
    <trd>w <ind>mi&eogonek;dzyczasie</ind></trd>
  </trdgrp>
  <trd lng="ro">&icirc;ntre timp</trd>
  <trd lng="ru">&c_t;&c_je;&c_m;
&c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_je;&c_m;</trd>
</drv>

<drv mrk="temp.liber0o">
  <kap>liber<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="T"/>o, dum kiu oni ne devas labori
      (precipe pri lernantoj a&ubreve; oficistoj):
      <ekz>
        kaj mi havos <klr>...</klr> du monatojn da liber<tld/>o
        <fnt><bib>Marta</bib></fnt>?
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="feri.0o">ferio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y; &c_ch;&c_a;&c_s;</trd>
  <trd lng="de">Freizeit</trd>
  <trd lng="fr">temps libre</trd>
  <trdgrp lng="he">
    <trd>&het;&vav;&pe;&shin;&he;<klr> (&mem;&ayin;&bet;&vav;&dalet;&he;
&alef;&vav; &mem;&lamed;&yod;&mem;&vav;&dalet;&yod;&fmem;)</klr></trd>,
    <trd>&pe;&gimel;&resh;&he;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szabadid&odblac;</trd>,
    <trd>szabads&aacute;g</trd>
  </trdgrp>
  <trd lng="nl">vrije <ind>tijd</ind></trd>
  <trdgrp lng="pl">
    <trd>urlop</trd>,
    <trd>wakacje</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>concediu</trd>,
    <trd>vacan&#x163;&#x103;</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_v;&c_o;&c_b;&c_o;&c_d;&c_n;&c_o;&c_je;
  &c_v;&c_r;&c_je;&c_m;&c_ja;</trd>
</drv>

