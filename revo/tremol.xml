<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tremol</rad>/o
</kap>
<drv mrk="tremol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="popl.0o">poplo</ref>
      (<trd lng="la">Populus tremula</trd>), kun
      cirklaj facile tremantaj folioj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Espe</trd>,
    <trd>Zitterpappel</trd>
  </trdgrp>
  <trd lng="en">aspen</trd>
  <trd lng="nl">ratelpopulier</trd>
  <trd lng="ru">&c_o;&c_s;&c_i;&c_n;&c_a;</trd>
</drv>
</art>
