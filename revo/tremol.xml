<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tremol.xml,v 1.20 2023/09/04 19:01:53 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tremol</rad>/o
</kap>
<drv mrk="tremol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="popl.0o">poplo</ref>
      (<trd lng="la">Populus tremula</trd>), kun
      mallongaj, pro longaj petioloj facile 
      <ref cel="trem.0i">trem</ref>antaj folioj:
      <ekz>
        la suno sin ka&scirc;is post malgranda <tld/>a arbaro
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 11a, p.
        59a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_ib;&c_n;&c_a;</trd>
  <trd lng="ca">tr&egrave;mol</trd>
  <trd lng="cs">osika obecn&aacute;</trd>
  <trdgrp lng="de">
    <trd>Espe</trd>,
    <trd>Zitterpappel</trd>
  </trdgrp>
  <trd lng="en">aspen</trd>
  <trd lng="fa">&a_S;&a_n;&a_w;&a_b;&a_r; &a_l;&a_r;&a_z;&a_A;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>peuplier tremble</trd>,
    <trd>tremble<klr> (arbre)</klr></trd>
  </trdgrp>
  <trd lng="hu">rezg&odblac; ny&aacute;r(fa)</trd>
  <trdgrp lng="ja">
    <trd>&#x30E4;&#x30DE;&#x30CA;&#x30E9;&#x30B7;</trd>,
    <trd>&#x30CF;&#x30B3;&#x30E4;&#x30CA;&#x30AE;</trd>,
    <trd>&#x30DD;&#x30D7;&#x30E9;</trd>
  </trdgrp>
  <trd lng="nl">ratelpopulier</trd>
  <trd lng="pl">osika</trd>
  <trd lng="pt">choupo-tremedor</trd>
  <trd lng="ru">&c_o;&c_s;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">osika</trd>
  <trd lng="uk">&#x43E;&#x441;&#x438;&#x43A;&#x430;</trd>
</drv>
</art>
