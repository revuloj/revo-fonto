<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: listel.xml,v 1.15 2022/03/22 18:26:51 revo Exp $">
<kap>
  <rad>listel</rad>/o <fnt><bib>MT</bib></fnt>
</kap>
<drv mrk="listel.0o">
  <kap><tld/>o</kap>
  <snc mrk="listel.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Mallar&gcirc;a randa elstara&jcirc;eto de arkitektura ornama&jcirc;o a&ubreve; de monero,
      medalo ktp:
      <ekz>
        faru &ccirc;irka&ubreve; <klr>[la tablo]</klr> manlar&gcirc;an <tld/>on
        <fnt><bib>MT</bib>, <lok>&Eli; 25:24</lok></fnt>.
      </ekz>
      <ekz>
        sur la muretoj, kiuj estis inter la <tld/>oj, estis leonoj, bovoj, kaj keruboj
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 7:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        listeloj, havantaj la lar&gcirc;on de unu manplato, estis &ccirc;e la muroj de la domo
        &ccirc;irka&ubreve;e
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 40:43</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="modlur.0o">modluro</ref>
    </refgrp>
  </snc>
  <trd lng="cs">v&yacute;stupek</trd>
  <trd lng="de">Leiste</trd>
  <trdgrp lng="fr">
    <trd>moulure</trd>,
    <trd>listel</trd>
  </trdgrp>
  <trd lng="hu">szeg&eacute;lyl&eacute;c</trd>
  <trdgrp lng="nl">
    <trd>lijst</trd>,
    <trd>richel</trd>
  </trdgrp>
  <trd lng="pl">listwa</trd>
  <trdgrp lng="pt">
    <trd>listel</trd>,
    <trd>filete <klr tip="amb">(arq.)</klr></trd>,
    <trd>listelo</trd>
  </trdgrp>
  <trd lng="ro">&#x219;ipc&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_i;&c_s;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_ja;&c_s;&c_o;&c_k;</trd>,
    <trd>&c_v;&c_y;&c_s;&c_t;&c_u;&c_p;</trd>,
    <trd>&c_v;&c_a;&c_l;&c_i;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_je;&c_b;&c_r;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>lem<klr>(vy&ccaron;nievaj&uacute;ci)</klr></trd>,
    <trd>obruba</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x456;&#x441;&#x442;&#x435;&#x43B;&#x44C;</trd>,
    <trd>&#x432;&#x438;&#x441;&#x442;&#x443;&#x43F;</trd>,
    <trd>&#x432;&#x430;&#x43B;&#x438;&#x43A;</trd>,
    <trd>&#x433;&#x440;&#x435;&#x431;&#x456;&#x43D;&#x44C;</trd>,
    <trd>&#x43E;&#x431;&#x456;&#x434;&#x43E;&#x43A; &#x43C;&#x43E;&#x43D;&#x435;&#x442;&#x438;</trd>
  </trdgrp>
</drv>
</art>
