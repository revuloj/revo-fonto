<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>listel</rad>/o
</kap>
<drv mrk="listel.0o">
  <kap><tld/>o</kap>
  <snc mrk="listel.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Mallar&gcirc;a randa elstara&jcirc;eto de arkitektura ornama&jcirc;o a&ubreve; de monero,
      medalo ktp:
      <ekz>
        faru &ccirc;irka&ubreve; <klr>[la tablo]</klr> mallar&gcirc;an <tld/>on
        <fnt><bib>MT</bib>, <lok>&Eli; 25:24</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="modlur.0o">modluro</ref>
    </refgrp>
  </snc>
  <trd lng="de">Leiste</trd>
  <trdgrp lng="fr">
    <trd>moulure</trd>,
    <trd>listel</trd>
  </trdgrp>
  <trd lng="hu">szeg&eacute;lyl&eacute;c</trd>
  <trdgrp lng="nl">
    <trd>lijst</trd>,
    <trd>richel</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>listel</trd>,
    <trd>filete <klr tip="amb">(arq.)</klr></trd>,
    <trd>listelo</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_i;&c_s;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_ja;&c_s;&c_o;&c_k;</trd>,
    <trd>&c_v;&c_y;&c_s;&c_t;&c_u;&c_p;</trd>,
    <trd>&c_v;&c_a;&c_l;&c_i;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_je;&c_b;&c_r;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
