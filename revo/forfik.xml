<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: forfik.xml,v 1.16 2018/10/17 18:10:20 revo Exp $">
<kap>
  <rad>forfikul</rad>/o
  <fnt><bib>Fab4</bib></fnt>
</kap>
<drv mrk="forfik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la ordo <ref tip="super"
      cel="dermap.0oj">dermapteroj</ref>
      (<trd lng="la">Forficula</trd>), kies abdomeno fini&gcirc;as per
      pin&ccirc;ilo:
      <ekz>
        vi devas vidi anka&ubreve; niajn malgrandajn <tld/>ojn
        <fnt>
          <bib>Fab4</bib>
          <lok>Sterkskarabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        Grandantila iktero: <klr>[&#8230;]</klr> inter predataj insektoj
        estas griloj, <tld/>oj, akridoj, blatoj, <klr>[&#8230;]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=333770">Grandantila iktero</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/4/46/Forficula-auricularia-04-fws.jpg/320px-Forficula-auricularia-04-fws.jpg"
      prm="CC0"
      ><tld/>o (Forficula auricularia), Eichsfeld, Turingio, Germanujo
        <fnt>
          <aut>Francisco Welter-Schultes, propra verko</aut>
          <lok><url ref="https://commons.wikimedia.org/wiki/File:Forficula-auricularia-04-fws.jpg"
              >https://commons.wikimedia.org/wiki/File:Forficula-auricularia-04-fws.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="ca">papaorelles</trd>
  <trdgrp lng="cs">
    <trd>st&rcaron;&iacute;havka</trd>,
    <trd>&scaron;kvor</trd>
  </trdgrp>
  <trd lng="de">Ohrwurm</trd>
  <trd lng="en">earwig</trd>
  <trdgrp lng="fr">
    <trd>forficule</trd>,
    <trd>perce-oreille</trd></trdgrp>
  <trd lng="hu">f&uuml;lbem&aacute;sz&oacute;</trd>
  <trd lng="nl">oorworm</trd>
  <trdgrp lng="pl">
    <trd>skorek</trd>,
    <trd>szczypawka</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>lacrainha</trd>,
    <trd>bicha-cadela</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_h;&c_o;&c_v;&c_jo;&c_r;&c_t;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>ucholak</trd>,
    <trd>&scaron;kvor</trd>
  </trdgrp>
</drv>
</art>
