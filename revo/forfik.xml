<vortaro>
<art mrk="$Id$">
<kap>
  <rad>forfikul</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="forfik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la ordo <ref tip="super" cel="dermap.0oj">dermapteroj</ref>
      (<trd lng="la">Forficula</trd>), kies abdomeno fini&gcirc;as per
      pin&ccirc;ilo.
    </dif>
  </snc>
  <trd lng="de">Ohrwurm</trd>
  <trdgrp lng="fr">
    <trd>forficule</trd>,
    <trd>perce-oreille</trd></trdgrp> 
  <trd lng="ru">&c_u;&c_h;&c_o;&c_v;&c_jo;&c_r;&c_t;&c_k;&c_a;</trd>
</drv>
</art>
