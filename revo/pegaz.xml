<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pegaz.xml,v 1.21 2023/10/20 14:25:20 revo Exp $">
<kap>
  <rad>Pegaz</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="pegaz.0o">
  <kap><tld/>o</kap>
  <snc mrk="pegaz.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Flugilhava <ref cel="cxeval.0o">&ccirc;evalo</ref>, simbolo de la poezia fantazio:
      <ekz>
        por montri laboron de poeto oni ofte uzas <tld/>on
        <fnt>
          <bib>KFl</bib>
          <lok>IV. El vivo de sistemoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        via task' konsistis nur el tio: &#x2AFD; Kolekti la sterkpomojn de <tld/>o
        <fnt>
          <bib>LSB</bib>
          <lok>Oni Malfermas La Butikon</lok>
        </fnt>!
      </ekz>
      <ekz>
        mi e&ccirc; imagos, ke mi rajdas sur la &ccirc;evalo <tld/>o
        <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 29a, p.
        220a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="centau.0o">centa&ubreve;ro</ref>,
      <ref cel="hipogrif.0o">hipogrifo</ref>
    </refgrp>
    <trd lng="be">&c_p;&c_e;&c_g;&c_a;&c_s;</trd>
    <trd lng="ca">Peg&agrave;s</trd>
    <trdgrp lng="fa">
      <trd>&a_A;&a_s;&a_b; &a_b;&a_A;&a_l;&a_d;&a_A;&a_r;</trd>,
      <trd>&f_p;&f_g;&a_A;&a_s;&a_w;&a_s;</trd>
    </trdgrp>
    <trd lng="pl">Pegaz</trd>
    <trd lng="ro">Pegas </trd>
  </snc>

  <snc mrk="pegaz.0o.AST">
    <mlg>Peg</mlg>
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
    <uzo tip="fak">AST</uzo>
   <dif>
     Nord-hemisfera kvadratforma
     <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref>
     (<trd lng="la">Pegasus</trd>).
   </dif>
   <trd lng="be">&c_P;&c_e;&c_g;&c_a;&c_s; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#x2019;&c_je;)</klr></trd>
   <trd lng="ca">el Peg&agrave;s</trd>
   <trdgrp lng="fa">
     <trd>&a_A;&a_s;&a_b; &a_b;&a_A;&a_l;&a_d;&a_A;&a_r;<klr tip="amb"> (&a_S;&a_w;&a_r;&a_t; &a_f;&a_l;&f_k;&f_y;)</klr></trd>,
     <trd>&a_f;&a_fatha;&a_r;&a_fatha;&a_s; &a_A;&a_ayn;&a_Z;&a_m;</trd>
   </trdgrp>
   <trd lng="pl">Pegaz <klr>(gwiazdozbi&oacute;r)</klr></trd>
   <trd lng="ro">Pegas <klr>(constela&#x21B;ie)</klr></trd>
 </snc>
  <trdgrp lng="cs">
    <trd>Pegas</trd>,
    <trd>Pegasos</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Pegasus</trd>,
    <trd>Pegasos</trd>
  </trdgrp>
  <trd lng="es">Pegaso</trd>
  <trd lng="fr">P&eacute;gase</trd>
  <trd lng="it">Pegaso</trd>
  <trdgrp lng="ja">
    <trd>&#x30DA;&#x30AC;&#x30BD;&#x30B9;</trd>,
    <trd>&#x30DA;&#x30AC;&#x30B9;&#x30B9;&#x5EA7;  <pr>&#x30DA;&#x30AC;&#x30B9;&#x30B9;&#x3056;</pr></trd>
  </trdgrp>
  <trd lng="nl">Pegasus</trd>
  <trd lng="pt">P&eacute;gaso</trd>
  <trd lng="sk">Pegas</trd>
  <trd lng="uk">&c_P;&c_je;&c_g;&c_a;&c_s;</trd>
</drv>
</art>

