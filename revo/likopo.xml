<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>likopodi</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="likopo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="likopo.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Lycopodium</trd>), sporoplanto kun skvamaj a&ubreve;
      pinglaj folietoj.
    </dif>
  </snc>
  <trd lng="de">B&auml;rlapp</trd>
</drv>
<drv mrk="likopo.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de sporoplantoj (<trd lng="la">Lycopodiaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">B&auml;rlappgew&auml;chse</trd>
</drv>
<drv mrk="likopo.0apulvoro">
  <kap><tld/>a pulvoro</kap>
  <snc>
    <dif>
      Pulvoro flaveta, kiun entenas la sporujoj de la <tld/>o kaj uzata
      en farmacio.
    </dif>
  </snc>
</drv>
</art>
