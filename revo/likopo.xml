<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>likopodi</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="likopo.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="likopo1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Lycopodium</trd>), sporoplanto kun skvamaj a&ubreve;
      pinglaj folietoj.
    </dif>
  </snc>
  <trd lng="de">B&auml;rlapp</trd>
  <trd lng="nl">wolfsklauw</trd>
  <trd lng="pt">licopodi&aacute;cea</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_a;&c_u;&c_n;</trd>,
    <trd>&c_l;&c_i;&c_k;&c_o;&c_p;&c_o;&c_d;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="likopo.0oplantoj">
  <kap>
    <tld/>oplantoj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Divizio de la <ref tip="super" cel="plant.0oj">plantoj</ref>
      (<trd lng="la">Lycopodiophyta</trd>), apartenanta al la
      angiohavaj <ref tip="vid" cel="spor.0oplantoj">sporoplantoj</ref>.
    </dif>
  </snc>
  <trd lng="en">club-mosses</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_a;&c_u;&c_n;&c_o;&c_v;&c_i;&c_d;&c_n;&c_y;&c_je;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_u;&c_n;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="likopo.0apulvoro">
  <kap>
    <tld/>a pulvoro
  </kap>
  <snc>
    <dif>
      Pulvoro flaveta, kiun entenas la sporujoj de la <tld/>o kaj uzata
      en farmacio.
    </dif>
  </snc>
  <trd lng="ru">&c_l;&c_i;&c_k;&c_o;&c_p;&c_o;&c_d;&c_i;&c_j; <klr>(&c_s;&c_p;&c_o;&c_r;&c_y; &c_p;&c_l;&c_a;&c_u;&c_n;&c_a;)</klr></trd>
</drv>
</art>
