<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: manat1.xml,v 1.15 2023/09/02 06:04:08 revo Exp $">
<kap>
  <rad>manat</rad>/o
</kap>

<drv mrk="manat1.0o">
  <kap><tld/>o</kap>
  <mlg>YAM</mlg>
  <mlg>TMM</mlg>
  <snc mrk="manat1.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="lst" cel="unu.mon0o" lst="voko:monunuoj">Monunuo</ref>, 
      uzata en Azerbaj&gcirc;ano kaj en Turkmenio:
      <ekz>
        azerbaj&gcirc;ana <tld/>o <klr>[&#x2026;]</klr> estas dividita je 100
        gepikoj (a&ubreve; gopikoj), la <tld/>o estis enkondukita en 1992
        kaj ekzistis &gcirc;is komence de 1994 paralele kun la sovetunia
        rublo, komence de 1994, la <tld/>o i&gcirc;is la sola landa valuto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=41943">Azerbaj&gcirc;ana manato</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_a;&c_n;&c_a;&c_t;</trd>
    <trd lng="de">Manat</trd>
    <trd lng="fa">&a_m;&a_n;&a_A;&a_t;</trd>
    <trd lng="id">manat</trd>
    <trd lng="nl">manat</trd>
    <trd lng="pl">manat</trd>
    <trd lng="pt">manat</trd>
    <trd lng="fr">manat</trd>
    <trd lng="en">manat</trd>
  </snc>
  <rim>
    <ref tip="hom" cel="manat.0o">Manato</ref>
    estas anka&ubreve; la nomo de mamulo vivanta en akvo.
  </rim>
  <trdgrp lng="ja">
    <trd>&#x30DE;&#x30CA;&#x30C6;&#x30A3;&#x30FC;</trd>,
    <trd>&#x30AB;&#x30A4;&#x30AE;&#x30E5;&#x30A6;</trd>
  </trdgrp>
  <trd lng="sk">druh morsk&eacute;ho bylino&zcaron;rav&eacute;ho cicavca</trd>
  <trd lng="cs">kapust&ncaron;&aacute;k</trd>
  <trd lng="uk">&#x43B;&#x430;&#x43C;&#x430;&#x43D;&#x442;&#x438;&#x43D; &#x430;&#x43C;&#x435;&#x440;&#x438;&#x43A;&#x430;&#x43D;&#x441;&#x44C;&#x43A;&#x438;&#x439;</trd>
</drv>
</art>

