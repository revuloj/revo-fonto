<kap>
  <rad>akvedukt</rad>/o
</kap>
<drv mrk="akvedu.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Masona&jcirc;a <ref tip="super" cel="konduk.0ilo.tubo">kondukilo</ref>, 
      subtera a&ubreve; surponta, kiu kondukas akvon inter du malproksimaj lokoj:
      <ekz>
        <nom>Canal del Mi&egrave;jorn</nom> posedas pli ol 350 kluzojn, pontojn, <tld/>ojn, inter kiuj
        kelkaj ne estis &scirc;an&gcirc;itaj ekde sia ekesto
        <fnt>
          <bib>Monato</bib>
          <aut>Marc Gassion</aut>
          <vrk><url ref="&Monato;/2003/007934.html">Aude</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_k;&c_v;&c_e;&c_d;&c_u;&c_k;</trd>,
    <trd>&c_v;&c_a;&c_d;&c_a;&c_p;&c_r;&c_a;&c_v;&c_o;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>akvadukt</trd>,
    <trd>antick&yacute; vodovod</trd>
  </trdgrp>
  <trd lng="de">Aqu&auml;dukt</trd>
  <trd lng="en">aqueduct</trd>
  <trd lng="es">acueducto</trd>
  <trd lng="fr">aqueduc</trd>
  <trd lng="he">&#x5D0;&#x5B7;&#x5DE;&#x5B7;&#x5EA; &#x5DE;&#x5B7;&#x5D9;&#x5B4;&#x5DD;</trd>
  <trd lng="hu">v&iacute;zvezet&eacute;k<klr tip="ind"> (aquaeductus)</klr></trd>
  <trd lng="nl">aquaduct</trd>
  <trd lng="pt">aqueduto</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_k;&c_v;&c_je;&c_d;&c_u;&c_k;</trd>,
    <trd>&c_v;&c_o;&c_d;&c_o;&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>akvadukt</trd>,
    <trd>antick&yacute; vodovod</trd>,
    <trd>vodovod</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x430;&#x43A;&#x432;&#x435;&#x434;&#x443;&#x43A;</trd>,
    <trd>&#x432;&#x43E;&#x434;&#x43E;&#x43F;&#x440;&#x43E;&#x432;&#x456;&#x434;</trd>,
    <trd>&#x432;&#x43E;&#x434;&#x43E;&#x433;&#x456;&#x43D;</trd>
  </trdgrp>
</drv>
