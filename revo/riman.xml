<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: riman.xml,v 1.16 2024/04/14 05:46:54 revo Exp $">
<kap>
  <rad>Riman</rad>/o
</kap>
<drv mrk="riman.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <dif>
      Germanlingve:
      <nom>Bernhard</nom> <trd lng="de">Riemann</trd>, 1826-1866, Germana
      <ref tip="lst" lst="voko:historiaj_personoj" cel="matema.0isto">matematikisto</ref>:
      <ekz>
        la teoremo de <tld/>o &dash; Ro&hcirc;o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=104267">Algebra kurbo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_R;&c_y;&c_m;&c_a;&c_n;</trd>
  <trd lng="bg">&c_B;&c_je;&c_r;&c_n;&c_h;&c_a;&c_r;&c_d; <ind>&c_R;&c_i;&c_m;&c_a;&c_n;</ind></trd>
  <trd lng="ca">Riemann</trd>
  <trd lng="cs">Riemann</trd>
  <trd lng="en">Riemann</trd>
  <trd lng="fr">Riemann</trd>
  <trd lng="hu">Riemann</trd>
  <trd lng="it">Riemann</trd>
  <trd lng="nl">Bernhard <ind>Riemann</ind></trd>
  <trd lng="pl">Riemann</trd>
  <trd lng="pt">Riemann</trd>
  <trd lng="ru">&c_R;&c_i;&c_m;&c_a;&c_n;</trd>
  <trd lng="sk">Riemann</trd>
  <trd lng="uk">&#x420;&#x456;&#x43C;&#x430;&#x43D; (&#x444;&#x456;&#x437;&#x438;&#x43A;)</trd>
  <trd lng="zh">&#x9ECE;&#x66FC; <pr>l&#xED;m&#xE0;n</pr></trd>
</drv>

<drv mrk="riman.r0a">
  <kap><tld lit="r"/>a</kap>
  <snc>
    <refgrp tip="vid">
      <ref cel="integr2.rimana0o"><tld lit="r"/>a integralo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_R;&c_y;&c_m;&c_a;&c_n;&c_a;</trd>,
    <trd>&c_r;&c_y;&c_m;&c_a;&c_n;&c_a;&c_w;</trd>
  </trdgrp>
  <trd lng="ca">de Riemann</trd>
  <trd lng="de">Riemannsche</trd>
  <trdgrp lng="en">
    <trd>Riemann['s]</trd>,
    <trd>Riemannian</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>de <ind>Riemann</ind></trd>,
    <trd>riemannien</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>Riemann-</trd>,
    <trd>Riemann-f&eacute;le</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>di <ind>Riemann</ind></trd>,
    <trd>riemanniano</trd>
  </trdgrp>
  <trd lng="nl">van <ind>Riemann</ind></trd>
  <trd lng="pl">Riemanna</trd>
  <trdgrp lng="ru">
    <trd>&c_R;&c_i;&c_m;&c_a;&c_n;&c_a;</trd>,
    <trd>&c_r;&c_i;&c_m;&c_a;&c_n;&c_o;&c_v;</trd>
  </trdgrp>
</drv>
</art>
