<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>Riman</rad>/o
</kap>
<drv mrk="riman.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Germanlingve:
      Bernhard <trd lng="de">Riemann</trd>, 1826-1866. Germana matematikisto.
    </dif>
  </snc>
  <trd lng="en">Riemann</trd>
  <trd lng="fr">Riemann</trd>
  <trd lng="nl">Bernhard <ind>Riemann</ind></trd>
  <trd lng="ru">&c_R;&c_i;&c_m;&c_a;&c_n;</trd>
</drv>
<drv mrk="riman.r0a">
  <kap><tld lit="r"/>a</kap>
  <snc>
    <refgrp tip="vid">
      <ref cel="integr2.rimana0o"><tld lit="r"/>a integralo</ref>.
    </refgrp>
  </snc>
    <trd lng="de">Riemannsche</trd>
    <trdgrp lng="en">
      <trd>Riemann['s]</trd>,
      <trd>Riemannian</trd></trdgrp> 
    <trdgrp lng="fr">
      <trd>de <ind>Riemann</ind></trd>,
      <trd>riemannien</trd></trdgrp> 
    <trd lng="nl">van <ind>Riemann</ind></trd>
    <trdgrp lng="ru">
      <trd>&c_R;&c_i;&c_m;&c_a;&c_n;&c_a;</trd>,
      <trd>&c_r;&c_i;&c_m;&c_a;&c_n;&c_o;&c_v;</trd></trdgrp> 
</drv>
</art>

