<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Riman</rad>/o
</kap>
<drv mrk="riman.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <dif>
      Germanlingve:
      Bernhard <trd lng="de">Riemann</trd>, 1826-1866. Germana matematikisto.
    </dif>
  </snc>
  <trd lng="be">&c_R;&c_y;&c_m;&c_a;&c_n;</trd>
  <trd lng="en">Riemann</trd>
  <trd lng="fr">Riemann</trd>
  <trd lng="hu">Riemann</trd>
  <trd lng="nl">Bernhard <ind>Riemann</ind></trd>
  <trd lng="pl">Riemann</trd>
  <trd lng="pt">Riemann</trd>
  <trd lng="ru">&c_R;&c_i;&c_m;&c_a;&c_n;</trd>
</drv>

<drv mrk="riman.r0a">
  <kap><tld lit="r"/>a</kap>
  <snc>
    <refgrp tip="vid">
      <ref cel="integr2.rimana0o"><tld lit="r"/>a integralo</ref>..
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_R;&c_y;&c_m;&c_a;&c_n;&c_a;</trd>,
    <trd>&c_r;&c_y;&c_m;&c_a;&c_n;&c_a;&c_w;</trd>
  </trdgrp>
  <trd lng="de">Riemannsche</trd>
  <trdgrp lng="en">
    <trd>Riemann['s]</trd>,
    <trd>Riemannian</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>de <ind>Riemann</ind></trd>,
    <trd>riemannien</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>Riemann-</trd>,
    <trd>Riemann-f&eacute;le</trd>
  </trdgrp>
  <trd lng="nl">van <ind>Riemann</ind></trd>
  <trd lng="pl">Riemanna</trd>
  <trdgrp lng="ru">
    <trd>&c_R;&c_i;&c_m;&c_a;&c_n;&c_a;</trd>,
    <trd>&c_r;&c_i;&c_m;&c_a;&c_n;&c_o;&c_v;</trd>
  </trdgrp>
</drv>
</art>
