<vortaro>
  <art mrk="$Id: kiot.xml,v 1.8 2023/12/03 09:58:49 afido Exp $">
    <kap>
      <rad>Kiot</rad>/o
    </kap>

<drv mrk="kiot.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">HIS</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">Urbo</ref> en <ref tip="malprt" cel="japan.0ujo">Japanujo</ref>,
      &ccirc;. 40&#x202F;km nordoriente de <ref cel="osak.0o">Osako</ref>,
      de 794 &gcirc;is 1868 <ref tip="super" cel="rezid.0ejo.sxtatestra">rezidejo</ref> de la japana
      imperiestro:
      <ekz>
        la japana bonzo <nom>Myoe</nom> &ccirc;e la templo <nom>Kozan-&gcirc;i</nom> en <tld/>o registradis
        siajn son&gcirc;ojn
        dum la jaroj 1191-1231
        <fnt>
          <aut>Reza Kheir-Khah</aut>
            <vrk><url ref="&Monato;/2000/007087.php">Mirinda dimensio de nia vivo</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 16</lok>
          </fnt>;
      </ekz>
      <ekz>
        subskribita en 1997 en la japana urbo <tld/>o, la protokolo difinas le&gcirc;e
        devigajn limigojn por CO<sub>2</sub>
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2005/008735.php">&Ccirc;u la tero fari&gcirc;os
          sa&ubreve;no?</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        en krom-dokumento por la <tld/>o-&scirc;tatoj trovi&gcirc;as la konkretaj ciferoj kaj estas
        indikita kiel celo la&ubreve;eble modesta alti&gcirc;o de la temperaturo
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2008/009411.php">Modera espero</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_K;&c_ib;&c_jo;&c_t;&c_a;</trd>
    <trd lng="de">Kyoto</trd>
    <trd lng="en">Kyoto</trd>
    <trd lng="fr">Kyoto</trd>
    <trd lng="ja">&#x4EAC;&#x90FD;&#x5E02; <pr>&#x304D;&#x3087;&#x3046;&#x3068;&#x3057;</pr></trd>
    <trd lng="nl">Kyoto</trd>
  </snc>
  <trd lng="uk">&#x41A;&c_ib;&#x43E;&c_t;&#x43E;</trd>
  <trdgrp lng="zh">
    <trd>&#x4EAC;&#x90FD; <pr>j&#x12B;ngd&#x16B;</pr></trd>,
    <trd>&#x4EAC;&#x90FD; <pr>J&#x12B;ngd&#x16B;</pr></trd>
  </trdgrp>
</drv>

