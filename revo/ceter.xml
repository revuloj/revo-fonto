<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>ceter</rad>/a
</kap>
<drv mrk="ceter.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      &Ccirc;iu restanta, &ccirc;iu alia; &ccirc;iu, kiun oni
      ankora&ubreve;
      ne menciis:
      <ekz>
        unuj jesas, aliaj neas, <tld/>aj silentas;
      </ekz>
      <ekz>
        la <tld/>aj lingvoj havas akcenton malregulan<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_s;&c_t;&c_a;&c_t;&c_n;&c_ib;</trd>,
    <trd>&c_ib;&c_n;&c_sh;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">ostatn&iacute;</trd>
  <trdgrp lng="de"><trd>&uuml;brig</trd>, <trd>restlich</trd></trdgrp>
  <trd lng="en">remaining</trd>
  <trdgrp lng="fr">
    <trd>autre</trd>,
    <trd>restant <klr tip="ind">(adj.)</klr></trd></trdgrp>
  <trd lng="es">dem&aacute;s</trd>
  <trdgrp lng="hu">
    <trd>egy&eacute;b</trd>,
    <trd>m&aacute;s</trd>
  </trdgrp>
  <trd lng="it">rimanente<ind> (agg.)</ind></trd>
  <trdgrp lng="pl">
    <trd>inny</trd>,
    <trd>pozosta&lstroke;y</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_ch;&c_i;&c_j;</trd>,
    <trd>&c_d;&c_r;&c_u;&c_g;&c_o;&c_j;</trd>,
    <trd>&c_o;&c_s;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">ostatn&yacute;</trd>
  <trdgrp lng="sv">
    <trd>&ouml;vrig</trd>,
    <trd>resterande</trd>
  </trdgrp>
  <trd lng="tp">awen</trd>
</drv>

<drv mrk="ceter.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Krom tio, aliflanke.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_r;&c_e;&c_sh;&c_t;&c_y;</trd>,
    <trd>&c_a;&c_d;&c_n;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>&uuml;brigens</trd>,
     <trd>im &Uuml;brigen</trd>,
     <trd>au&szlig;erdem</trd></trdgrp>
  <trd lng="es">adem&aacute;s</trd>
  <trdgrp lng="fr">
    <trd>d'<ind>ailleurs</ind></trd>,
    <trd>au <ind>reste</ind></trd>,
    <trd>du <ind>reste</ind></trd>
  </trdgrp>  
  <trdgrp lng="hu">
    <trd>egy&eacute;bk&eacute;nt</trd>,
    <trd>egy&eacute;bir&aacute;nt</trd>,
    <trd>k&uuml;l&ouml;nben</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>peraltro</trd>,
    <trd>inoltre</trd>,
    <trd>poi<ind> (inoltre)</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>opr&oacute;cz tego</trd>,
    <trd>poza tym</trd>,
    <trd>zreszt&aogonek;</trd>,
    <trd>dodatkowo</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_p;&c_r;&c_o;&c_ch;&c_je;&c_m;</trd>
  <trd lng="sk">napokon</trd>
  <trd lng="sv">f&ouml;r <ind>&ouml;vrigt</ind></trd>
</drv>

<drv mrk="ceter.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;io restanta:
      <ekz>faru vian aferon, Dio zorgos
      <tld/>on<fnt><bib>PrV</bib></fnt>.</ekz> 
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_s;&c_t;&c_a;&c_t;&c_n;&c_ja;&c_je;
    <klr>(&c_n;&c_a;&c_z;.)</klr></trd>,
    <trd>&c_ib;&c_n;&c_sh;&c_a;&c_je;
    <klr>(&c_n;&c_a;&c_z;.)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Rest</trd>,
     <trd>&Uuml;briges</trd></trdgrp>
  <trd lng="es">resto</trd>
  <trd lng="fr">reste <klr tip="ind">(tout ce qui reste)</klr></trd>
  <trdgrp lng="hu">
    <trd>egy&eacute;b</trd>,
    <trd>t&ouml;bbi</trd>
  </trdgrp>
  <trd lng="it">rimanente<ind> (sost.)</ind></trd>
  <trdgrp lng="pl">
    <trd>reszta</trd>,
    <trd>pozosta&lstroke;o&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_ch;&c_je;&c_je;
    <klr>(&c_s;&c_u;&c_shch;.)</klr></trd>,
    <trd>&c_o;&c_s;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_je;
    <klr>(&c_s;&c_u;&c_shch;.)</klr></trd>
  </trdgrp>
  <trd lng="sk">zvy&scaron;ok</trd>
  <trd lng="sv">rest</trd>
</drv>
</art>
