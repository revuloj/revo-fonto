<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kanad.xml,v 1.19 2021/05/12 09:57:47 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>Kanad</rad>/o <fnt><bib>FK</bib></fnt>
</kap>

<drv mrk="kanad.K0o">
  <kap>
    <ofc>8</ofc><tld/>o
    <fnt><bib>RLN</bib></fnt>
  </kap>
  <mlg kod="ISO-3166">CA</mlg>
  <snc mrk="kanad.K0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Granda <ref tip="malprt" cel="amerik.Nord0o">Nordamerika</ref>
      <ref tip="lst" cel="sxtat.0o" lst="voko:&scirc;tatoj">&scirc;tato</ref> kun
      &ccirc;efurbo Otavo,
      limanta kun <ref tip="vid" cel="uson.U0o">Usono</ref>
      kaj oceanoj:
      <ref tip="vid" cel="atlant.A0o">Atlantiko</ref>,
      <ref tip="vid" cel="ocean.ArktaO0o">Arkta</ref>
      kaj <ref tip="vid" cel="pacifik.P0o">Pacifiko</ref>:
      <ekz>
        per vapor&scirc;ipoj kaj fervojoj alvenas sen&ccirc;ese novaj 
        homoj el plej diversaj punktoj de la Unuigitaj &Scirc;tatoj kaj
        <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        Svisujo, Belgujo, A&ubreve;strujo, <tld/>o, Meksiko, Peruo
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Homaranismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estas lando &ccirc;efe formita de enmigrintoj
        <fnt>
          <bib>Monato</bib>
          <vrk><url
          ref="http://steloj.de/esperanto/monato1/monato-9401-kanado.txt">Kanado</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_K;&c_a;&c_n;&c_a;&c_d;&c_a;</trd>
    <trd lng="bg">&c_K;&c_a;&c_n;&c_a;&c_d;&c_a;</trd>
    <trd lng="de">Kanada</trd>
    <trd lng="en">Canada</trd>
    <trd lng="es">Canad&aacute;</trd>
    <trd lng="fa">&f_k;&a_A;&a_n;&a_A;&a_d;&a_A;</trd>
    <trd lng="fr">Canada</trd>
    <trd lng="fy">Kanada</trd>
    <trd lng="id">Kanada</trd>
    <trd lng="io">Kanada</trd>
    <trd lng="nl">Canada</trd>
    <trd lng="no">Canada</trd>
    <trd lng="pl">Kanada</trd>
    <trd lng="pt">Canad&aacute;</trd>
    <trd lng="ro">Canada</trd>
    <trd lng="tp">ma Kanata</trd>
    <trd lng="vo">Kanad&auml;n</trd>
  </snc>
  <refgrp tip="prt">
  <ref cel="albert1.A0o">Alberto</ref>, 
  <ref cel="kolumbi.britaK0o">Brita Kolumbio</ref>, 
  <ref cel="insul.I0odePrincoEduardo">Insulo de Princo
    Eduardo</ref>,
  <ref cel="kebeki.K0o">Kebekio</ref>, 
  <ref cel="manitob.M0o">Manitobo</ref>,
  <ref cel="brunsvik.novaB0o">Nov-Brunsviko</ref>,
  <ref cel="skoti.novaS0o">Nov-Skotio</ref>,
  <ref cel="land.Nov0okajLabradoro">Novlando kaj Labradoro</ref>,
  <ref cel="ontari.O0o">Ontario</ref>,
  <ref cel="saskacxevan.S0o">Saska&ccirc;evano</ref>
</refgrp>
</drv>

<drv mrk="kanad.0a">
   <kap><tld lit="k"/>a</kap>
   <snc mrk="kanad.0a.GOEG">
     <dif>
       Rilata al <tld/>o:
      <ekz>
        la <tld lit="k"/>a &scirc;tato
        <fnt>
          <bib>Monato</bib>
          <aut>Brian Moon</aut>
          <vrk><url
          ref="https://www.monato.be/2003/007984.html">Manipulado
          duobla</url></vrk>
        </fnt>.
      </ekz>
     </dif>
      <trd lng="de">kanadisch</trd>
   </snc>
   <trdgrp lng="be">
     <trd>&c_k;&c_a;&c_n;&c_a;&c_d;&c_z;&c_k;&c_ib;</trd>,
     <trd>&c_k;&c_a;&c_n;&c_a;&c_d;&c_y;&c_j;&c_s;&c_k;&c_ib;</trd>
   </trdgrp>
   <trd lng="en">Canadian</trd>
   <trdgrp lng="fr"> 
    <trd>canadien<klr> (adj.)</klr></trd>
    <trd>canadienne<klr> (adj.)</klr></trd>
   </trdgrp>
   <trd lng="fy">Kanadeesk</trd>
   <trd lng="io">Kanadana</trd>
   <trd lng="nl">Canadees</trd>
   <trd lng="pl">kanadyjski</trd>     
   <trd lng="ro">canadian</trd>
   <trd lng="vo">Kanad&auml;nik</trd>
</drv>

<drv mrk="kanad.0ano">
   <kap><tld lit="k"/>ano</kap>
   <snc mrk="kanad.0ano.GEOG">
     <dif>
       Civitano de <tld/>o:
      <ekz>
        inter la <tld lit="k"/>anoj dancis kaj ludis inuitoj
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2005/008666.php">Folklora
          festivalo en Kaustinen</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve; optimismo-skalo, irlandanoj trovi&gcirc;as sur preska&ubreve; la sama
        &scirc;tupo kiel a&ubreve;stralianoj kaj <tld lit="k"/>anoj
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2011/010374.php">Plej
          optimisma popolo</url></vrk>
        </fnt>.
      </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_k;&c_a;&c_n;&c_a;&c_d;&c_z;&c_je;&c_c;</trd>,
     <trd>&c_k;&c_a;&c_n;&c_a;&c_d;&c_y;&c_je;&c_c;</trd>
   </trdgrp>
   <trd lng="de">Kanadier</trd>
  <trd lng="en">Canadian</trd>
   <trdgrp lng="fr"> 
    <trd>Canadien<klr> (subst.)</klr></trd>
    <trd>Canadienne<klr> (subst.)</klr></trd>
   </trdgrp>
   <trd lng="fy">Kanadees</trd>
   <trd lng="io">Kanadano</trd>
   <trd lng="nl">Canadees</trd>
   <trd lng="pl">Kanadyjczyk</trd>     
   <trd lng="ro">canadian</trd>
   <trd lng="tp">jan Kanata</trd>
   <trd lng="vo">Kanad&auml;nan</trd>
</drv>

</art>

