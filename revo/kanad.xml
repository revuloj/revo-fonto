<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kanad</rad>/o
  <ofc>8</ofc>
</kap>

<drv mrk="kanad.0o">
  <kap>
    <tld lit="K"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="kanad.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Granda <ref tip="malprt" cel="amerik.Nord0o">Nordamerika</ref>
      lando kun &ccirc;efurbo Otavo.
      ISO-3166 kodo: CA.
      &Gcirc;i limas kun
      <ref tip="vid" cel="uson.0o">Usono</ref>
      kaj oceanoj:
      <ref tip="vid" cel="atlant.0o">Atlantiko</ref>,
      <ref tip="vid" cel="ocean.ArktaO0o">Arkta</ref>
      kaj <ref tip="vid" cel="pacifik.0o">Pacifiko</ref>.
    </dif>
    <trd lng="bg">&c_K;&c_a;&c_n;&c_a;&c_d;&c_a;</trd>
    <trd lng="de">Kanada</trd>
    <trd lng="en">Canada</trd>
    <trd lng="fa">&f_k;&a_A;&a_n;&a_A;&a_d;&a_A;</trd>
    <trd lng="fr">Canada</trd>
    <trd lng="pl">Kanada</trd>
  </snc>
</drv>

</art>

