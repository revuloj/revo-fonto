<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>konk</rad>/o
</kap>
<drv mrk="konk.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="konk.0o.tego">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Kalka tega&jcirc;o de iuj
      <ref tip="malprt" cel="molusk.0oj">moluskoj</ref>:
      <ekz>
        <tld/>o de heliko, mitulo, ostro.
      </ekz>
      <ekz>
        en la kuirejo trovi&gcirc;is abunda provizo da ranoj sur rostostango,
        limakaj <tld/>oj kun far&ccirc;o el infanaj fingroj kaj salatoj el
        semoj de fungoj
        <fnt>(<aut>Andersen, trad. Zamenhof</aut>: <vrk>Fabeloj,
        Monteto de elfoj</vrk>)</fnt>
      </ekz>
      <ekz>
        li pripalpu la malgrandan konkon de la limako
        <fnt>(<aut>Andersen, trad. Zamenhof</aut>: <vrk>Fabeloj,
        Feli&ccirc;a familio</vrk>)</fnt>
      </ekz>
      <trdgrp lng="fr">
        <trd>coquille</trd>
      </trdgrp>
    </dif>
  </snc>
  <snc mrk="konk.0o.molusko">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Molusko havanta <tld/>on:
      <ekz>
        la tegmento estas farita el konkoj kiuj sin fermas kaj malfermas
        la&ubreve; la fluo de la akvo
        <fnt>(<aut>Andersen, trad. Zamenhof</aut>: <vrk>Fabeloj,
        La virineto de maro</vrk>)</fnt>
      </ekz>
      <ekz>
        en &ccirc;iu konko ku&scirc;as brilantaj perloj el
        kiuj &ccirc;iu sola jam estus efektiva beliga&jcirc;o en la krono
        de re&gcirc;ino
        <fnt>(<aut>Andersen, trad. Zamenhof</aut>: <vrk>Fabeloj,
        La virineto de maro</vrk>)</fnt>
      </ekz>
    </dif>
    <ref tip="malprt" cel="valv.du0uloj">duvalvuloj</ref>
    <trdgrp lng="fr">
      <trd>coquillage</trd>
    </trdgrp>
  </snc>
  <snc mrk="konk.0o.konkoforma">
    <dif>
      Diversaj <tld/>oformaj objektoj:
      <ekz>
        <tld/>o de la orelo;
      </ekz>
      <ekz>
        marmora <tld/>ego de fontano.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>coquille</trd>
    </trdgrp>
  </snc>
  <trd lng="de">Muschel</trd>
  <trd lng="nl">schelp</trd>
  <trd lng="en">shell</trd>
  <trd lng="ru">&c_r;&c_a;&c_k;&c_o;&c_v;&c_i;&c_n;&c_a;</trd>
</drv>

<drv mrk="konk.bor0o">
  <kap>
    bor<tld/>o
  </kap>
  <snc>
    <ref tip="dif" cel="folad.0o">Folado</ref>
  </snc>
</drv>

<drv mrk="konk.klap0oj">
  <kap>
    klap<tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="lameli.0oj">Lamelibrankoj</ref>.
    </dif>
  </snc>
</drv>

</art>
