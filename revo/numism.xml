<art mrk="$Id: numism.xml,v 1.19 2024/01/29 15:59:31 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>numismatik</rad>/o
</kap>
<drv mrk="numism.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:sciencoj" cel="scienc.0o.fako">Scienco</ref>
      pri monoj, moneroj kaj medaloj:
      <ekz>
        aliaj modernaj monformoj pristudataj de la <tld/>o estus la &ccirc;eko, la monpapero
        a&ubreve; monbileto, la akcio kaj la obligacio, la kreditkarto kaj la &jcirc;etono
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=125919">Numismatiko</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Istv&aacute;n Kov&aacute;cs</nom> <klr>[&#x2026;]</klr> studis <tld/>on anka&ubreve; en
        <nom>Strassburg</nom> kaj <nom>Berlin</nom>, li doktori&gcirc;is en 1910
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=257118">Istv&aacute;n Kov&aacute;cs (arkeologo)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_u;&c_m;&c_ib;&c_z;&c_m;&c_a;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="cs">numismatika</trd>
  <trd lng="de">Numismatik</trd>
  <trd lng="el">&ny;&omikron;&my;&jota;&sigma;&my;&alfa;&tau;&jota;&kappa;&eta_ton;</trd>
  <trd lng="fr">numismatique</trd>
  <trd lng="hu">numizmatika</trd>
  <trd lng="ja">&#x53E4;&#x92AD;&#x5B66;  <pr>&#x3053;&#x305B;&#x3093;&#x304C;&#x304F;</pr></trd>
  <trd lng="nl">numismatiek</trd>
  <trd lng="pl">numizmatyka</trd>
  <trd lng="pt">numism&aacute;tica</trd>
  <trd lng="ro">numismatic&abreve;</trd>
  <trd lng="ru">&c_n;&c_u;&c_m;&c_i;&c_z;&c_m;&c_a;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">numizmatika</trd>
  <trd lng="uk">&#x43D;&#x443;&#x43C;&#x456;&#x437;&#x43C;&#x430;&#x442;&#x438;&#x43A;&#x430;</trd>
  <trdgrp lng="zh">
    <trd>&#x8CA8;&#x5E63;&#x5B78; <pr>hu&#xF2;b&#xEC;xu&#xE9;</pr></trd>,
    <trd>&#x8D27;&#x5E01;&#x5B66; <pr>hu&#xF2;b&#xEC;xu&#xE9;</pr></trd>
  </trdgrp>
</drv>
</art>
