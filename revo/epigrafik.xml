<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>epigrafik</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="epigrafik.0o">
  <kap><tld/>o</kap>
  <snc mrk="epigrafik.0o.ARKE">
    <uzo tip="fak">ARKE</uzo>
    <dif>
      Scienco pri <ref tip="vid" cel="epigra.0o.ARKE">epigrafoj</ref>,
      historiaj skriba&jcirc;oj sur monumentoj, en &scirc;tonoj, 
      metaloj a&ubreve; aliaj malmolaj materialoj:
      <ekz>
        F.A. Pennacchietti <klr>...</klr> okupi&gcirc;is pri greka 
        <tld/>o, semida <tld/>o, semida historio kaj kompara 
        lingvistiko
        <fnt>
          <aut>red. D. Blanke</aut>,
          <vrk>Personoj</vrk>,
          <lok>Informilo por Interlingvistoj, 2007:2-3 (61-62)</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">epigrafia</trd>
    <trd lng="fr">&eacute;pigraphie</trd>
    <trd lng="pl">epigrafia</trd>
  </snc>
</drv>

<drv mrk="epigrafik.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="epigrafik.0isto.ARKE">
    <uzo tip="fak">ARKE</uzo>
    <dif>
      Fakulo pri <ref tip="vid" cel="epigrafik.0o.ARKE"><tld/>o</ref>:
      <ekz>
        li pasigis tempon anka&ubreve; en la sikula regiono kaj kiel
        vaste 
        konata <tld/>isto li &ccirc;ie ser&ccirc;adis malnovajn
        skriba&jcirc;ojn
        <fnt>
          <aut>Haszpra O.</aut>,
          <vrk><url ref="http://espleono.uw.hu/haspra.html">
          La praa runskribo de la hungaroj</url></vrk>,
          <lok>Scienca Revuo, 2001:3</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">epigrafista</trd>
  <trd lng="fr">&eacute;pigraphiste</trd>
</drv>
</art>

