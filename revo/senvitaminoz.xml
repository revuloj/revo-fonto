<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: senvitaminoz.xml,v 1.4 2023/08/05 18:28:11 revo Exp $">
<kap>
  <rad>senvitaminoz</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="senvitaminoz.0o">
  <kap><tld/>o</kap>
  <snc mrk="senvitaminoz.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="san.mankomal0o.MED">Mankomalsano</ref>
      aperanta pro nesufi&ccirc;o de iuj
      <ref tip="vid" cel="vitamin.0o.BIO">vitaminoj</ref>:
      <ekz>
        profunda <tld/>o
        <fnt>
          <aut>F. C. Xavier, trad. C. de Almeida Wutke</aut>,
          <vrk>En pli granda mondo</vrk>,
          <lok>1977</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_v;&c_ib;&c_t;&c_a;&c_m;&c_ib;&c_n;&c_o;&c_z;</trd>
  <trd lng="en">avitaminosis</trd>
  <trd lng="fr">avitaminose</trd>
  <trd lng="pl">awitaminoza</trd>
  <trdgrp lng="uk">
    <trd>&c_a;&c_v;&c_ib;&c_t;&c_a;&#x43C;&c_ib;&#x43D;&#x43E;&c_z;</trd>,
    <trd>&c_v;&c_ib;&c_t;&c_a;&#x43C;&c_ib;&#x43D;&#x43D;&c_a; &#x43D;&c_je;&c_d;&#x43E;&c_s;&c_t;&c_a;&c_t;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>
  </trdgrp>
</drv>
</art>

