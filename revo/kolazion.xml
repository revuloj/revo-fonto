<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kolazion.xml,v 1.15 2023/11/27 09:25:32 revo Exp $">
<kap>
  <rad>kolazion</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kolazion.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="kolazion.0i.KUI">
    <dif>
      <ref tip="super" cel="mangx.0eti">Man&gcirc;eti</ref>
      iam inter tag- kaj vesperman&gcirc;o:
      <ekz>
        li kaj <nom>Don Quijote</nom> <klr>[&#8230;]</klr> matenman&gcirc;is,
        tagman&gcirc;is, <tld/>is kaj versperman&gcirc;is per unu fojo
        <fnt><bib>DonKihxoto</bib>
        <lok>&ccirc;apitro 19a, p. 135a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_v;&c_ja;&c_ch;&c_o;&c_r;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_u;&c_d;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">berenar</trd>
  <trd lng="cs">sva&ccaron;it <klr>(odpoled.)</klr></trd>
  <trd lng="de">vespern</trd>
  <trdgrp lng="es">
    <trd>merendar</trd>,
    <trd>tomar la merienda</trd>
  </trdgrp>
  <trd lng="fr">go&ucirc;ter<klr tip="ind"> (faire un go&ucirc;ter)</klr></trd>
  <trdgrp lng="gl">
    <trd>merendar</trd>,
    <trd>tomar a merenda</trd>
  </trdgrp>
  <trd lng="it">fare merenda</trd>
  <trdgrp lng="ja">
    <trd>&#38291;&#39135;&#12377;&#12427;  <pr>&#12363;&#12435;&#12375;&#12423;&#12367;&#12377;&#12427;</pr></trd>,
    <trd>&#36605;&#39135;&#12434;&#12392;&#12427;  <pr>&#12369;&#12356;&#12375;&#12423;&#12367;&#12434;&#12392;&#12427;</pr></trd>,
    <trd>&#12362;&#12420;&#12388;&#12434;&#39135;&#12409;&#12427;  <pr>&#12362;&#12420;&#12388;&#12434;&#12383;&#12409;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>lanchar</trd>,
    <trd>comer o lanche</trd>,
    <trd>merendar</trd>,
    <trd>comer a merenda</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_d;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">olovrantova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_ji;&c_s;&c_t;&c_i; &c_p;&c_ib;&c_d;&c_v;&c_je;&c_ch;&c_ib;&c_r;&c_o;&c_k;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_v;&c_je;&c_ch;&c_ib;&c_r;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="kolazion.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="kolazion.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Posttagmeza
      <ref tip="super" cel="mangx.inter0eto">interman&gcirc;eto</ref>:
      <ekz>
        iliaj doktaj vortoj kaj paroloj alternis brue-flue kun la &scirc;macoj kaj moderaj
        borborigmoj de la <tld/>o
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 3</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="mangx.te0o">teman&gcirc;o</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_v;&c_ja;&c_ch;&c_o;&c_r;&c_a;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_u;&c_d;&c_z;&c_je;&c_n;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>col&middot;laci&oacute;</trd>,
    <trd>berenar</trd>
  </trdgrp>
  <trd lng="cs">sva&ccaron;ina <klr>(odpol.)</klr></trd>
  <trd lng="de">Vesper <klr>(Essen)</klr></trd>
  <trd lng="es">merienda</trd>
  <trd lng="fr">gouter<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="gl">merenda</trd>
  <trd lng="it">merenda</trd>
  <trdgrp lng="ja">
    <trd>&#38291;&#39135;  <pr>&#12363;&#12435;&#12375;&#12423;&#12367;</pr></trd>,
    <trd>&#36605;&#39135;  <pr>&#12369;&#12356;&#12375;&#12423;&#12367;</pr></trd>,
    <trd>&#12362;&#12420;&#12388;</trd>
  </trdgrp>
  <trd lng="pl">podwieczorek</trd>
  <trdgrp lng="pt">
    <trd>lanche</trd>,
    <trd>merenda</trd>,
    <trd>caf&eacute; da tarde [Brasil]</trd>
  </trdgrp>
  <trd lng="ro">gustare</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_d;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">olovrant</trd>
  <trd lng="uk">&c_p;&c_ib;&c_d;&c_v;&c_je;&c_ch;&c_ib;&c_r;&c_o;&c_k;</trd>
</drv>
<rim>
  La radiko estas malofta. &Gcirc;i aperis en PIV1, sed en PIV2 &gcirc;i jam
  malestas.
  <aut>Sergio Pokrovskij</aut>
</rim>
<adm>
  La vortoj sxajnas prenitaj de la francaj "collation",
  "collationner", kiuj tamen havas alian sencon,
  nome "mangxeto", kaj speciale "mangxeto, anstatauxanta
  la vesperan mangxon".
  <aut>MB</aut>
</adm>
</art>
