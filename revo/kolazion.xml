<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kolazion</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kolazion.0i">
  <kap><tld/>i</kap>
  <snc>
    <dif>
      Preni man&gcirc;on iam inter tag- kaj vesper-man&gcirc;o:
      <ekz>
        li kaj Don Quijote <klr>...</klr> matenman&gcirc;is, tagman&gcirc;is, <tld/>is
        kaj versperman&gcirc;is per unu fojo<fnt><bib>DonKihxoto</bib>
        <lok>&ccirc;apitro 19a, p. 135a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>gouter<klr> (faire un gouter)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="kolazion.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Posttagmeza, ordinare eta <ref tip="super"
      cel="mangx.0o">man&gcirc;o</ref>.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>gouter<klr> (subst.)</klr></trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
