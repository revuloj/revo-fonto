<kap>
<rad>bol&scirc;evist</rad>/o
</kap>
<drv mrk="bolsxe1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Adepto de <ref cel="bolsxe.0o">bol&scirc;evismo</ref>:
      <ekz>
        la Oktobra revolucio estis parto de la Rusia revolucio, la <tld/>oj sub gvido de
        <nom>Lenin</nom> perforte &ccirc;esigis la postcaran demokration kaj komencis konstrui novan,
        socialisme orientitan &scirc;taton
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=13547">Oktobra Revolucio</url></lok>
        </fnt>;
      </ekz>      
      <ekz>
        <nom>Jaroslav Ha&scaron;ek</nom> <klr>[&#x2026;]</klr>estis kaptita de rusoj, li edzi&gcirc;is en Rusio
        duafoje, <klr>[&#x2026;]</klr> fari&gcirc;is <tld/>a komisaro, sed post la milito li
        revenis denove en Pragon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1966">Jaroslav Ha&scaron;ek</url></lok>
        </fnt>.
      </ekz>  
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_l;&c_mol;&c_sh;&c_a;&c_v;&c_ib;&c_k;</trd>
  <trdgrp lng="de">
    <trd>Bolschewik</trd>,
    <trd>Bolschewist</trd>
  </trdgrp> 
  <trd lng="en">Bolshevik</trd>
  <trd lng="es">bolchevique</trd>
  <trdgrp lng="fr">
    <trd>bolchevik</trd>,
    <trd>bolcheviste</trd>
  </trdgrp> 
  <trd lng="he">&#x5D1;&#x5D5;&#x5DC;&#x5E9;&#x5D1;&#x5D9;&#x5E7;</trd>
  <trd lng="hu">bolsevik</trd>
  <trd lng="ja">&#x30DC;&#x30EB;&#x30B7;&#x30A7;&#x30D3;&#x30AD;</trd>
  <trd lng="pl">bolszewik</trd>
  <trd lng="ru">&c_b;&c_o;&c_l;&c_mol;&c_sh;&c_je;&c_v;&c_i;&c_k;</trd>
  <trd lng="sk">bol&scaron;evik</trd>
  <trd lng="uk">&#x431;&#x456;&#x43B;&#x44C;&#x448;&#x43E;&#x432;&#x438;&#x43A;</trd>
</drv>
