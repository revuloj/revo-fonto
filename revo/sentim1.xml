<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sentim1.xml,v 1.22 2024/11/04 07:26:31 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>sentimental</rad>/a
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="sentim1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Havanta a&ubreve; afektanta romanecan sentemon:
      <ekz>
        mi adoras palajn viza&gcirc;ojn kaj <tld/>ajn okulojn &ccirc;e la virinoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la junaj fra&ubreve;linoj &ccirc;ie ludadis la <nom>Prie're d'une vierge</nom>, tiun <tld/>an
        kompona&jcirc;on, kies tonoj melankolie kunfandi&gcirc;adis kun <klr>[&#8230;]</klr> la
        knabinaj sopiroj, elflugantaj el la brusto
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i vi&scirc;is la okulojn kaj levis sin kaj provis rideti &ndash; ni ne fari&gcirc;u <tld/>aj
        <fnt>
          <bib>Tor</bib>
          <lok>II.</lok>
        </fnt>!
      </ekz>
      <ekz>
        li <ind><tld/>e</ind> levis la okulojn kaj mieltone flustris al
        <nom>Margarita</nom>
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 24a, p. xa</lok>
        </fnt>.
        <trd lng="ca">sentimentalment</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="afekt.0a">afekta</ref>,
      <ref cel="emoci.0a">emocia</ref>,
      <ref cel="sent.0ema">sentema</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_e;&c_n;&c_t;&c_y;&c_m;&c_je;&c_n;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_u;&c_r;&c_a;&c_z;&c_mol;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_ch;&c_u;&c_l;&c_mol;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_ch;&c_u;&c_l;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">sentimental</trd>
  <trdgrp lng="cs">
    <trd>citov&yacute;</trd>,
    <trd>p&rcaron;ecitliv&ecaron;l&yacute;</trd>,
    <trd>sentiment&aacute;ln&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sentimental</trd>,
    <trd>r&uuml;hrselig</trd>,
    <trd>&uuml;bertrieben <ind>gef&uuml;hlvoll</ind></trd>,
    <trd>schmalzig <klr>(sentimental)</klr></trd>
  </trdgrp>
  <trd lng="en">sentimental</trd>
  <trdgrp lng="fr">
    <trd>romanesque<klr> (sentimental)</klr></trd>,
    <trd>sentimental</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;rzelmes</trd>,
    <trd>&eacute;rzelg&odblac;s</trd>,
    <trd>szentiment&aacute;lis</trd>
  </trdgrp>
  <trd lng="it">sentimentale</trd>
  <trdgrp lng="ja">
    <trd>&#24863;&#20663;&#30340;&#12394;  <pr>&#12363;&#12435;&#12375;&#12423;&#12358;&#12390;&#12365;&#12394;</pr></trd>,
    <trd>&#24773;&#12395;&#12418;&#12429;&#12356;  <pr>&#12376;&#12423;&#12358;&#12395;&#12418;&#12429;&#12356;</pr></trd>,
    <trd>&#12475;&#12531;&#12481;&#12513;&#12531;&#12479;&#12523;&#12394;  <pr>&#12379;&#12435;&#12385;&#12417;&#12435;&#12383;&#12427;&#12394;</pr></trd>
  </trdgrp>
  <trd lng="nl">sentimenteel</trd>
  <trdgrp lng="pl">
    <trd>sentymentalny</trd>,
    <trd>uczuciowy</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_n;&c_t;&c_i;&c_m;&c_je;&c_n;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>citov&yacute;</trd>,
    <trd>precitliven&yacute;</trd>,
    <trd>sentiment&aacute;lny</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_je;&c_n;&c_t;&c_i;&c_m;&c_je;&c_n;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#22810;&#24773; <pr>du&omacron;q&iacute;ng</pr></trd>,
    <trd>&#24863;&#20260; <pr>g&#462;nsh&amacron;ng</pr></trd>,
    <trd>&#24863;&#20663; <pr>g&#462;nsh&amacron;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sentim1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Forta <ref tip="super" cel="sent.0emo">sentemo</ref>, sinlaso al
      emocioj, aparte al amaj kaj dol&ccirc;aj imagoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="afekci.0o">afekcio</ref>,
      <ref cel="emoci.0o">emocio</ref>,
      <ref cel="romant.0ismo">romantikismo</ref>,
      <ref cel="sentim.0o">sentimento</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_n;&c_t;&c_y;&c_m;&c_e;&c_n;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="ca">sentimentalisme</trd>
  <trdgrp lng="de">
    <trd>Sentimentalit&auml;t</trd>,
    <trd>R&uuml;hrseligkeit</trd>
  </trdgrp>
  <trd lng="en">sentimentality</trd>
  <trdgrp lng="fr">
    <trd>sentimentalisme</trd>,
    <trd>sentimentalit&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;rzelmess&eacute;g</trd>,
    <trd>&eacute;rzelg&odblac;ss&eacute;g</trd>,
    <trd>szentimentalizmus</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>sentimentalismo</trd>,
    <trd>sentimentalita</trd>
  </trdgrp>
  <trd lng="ja">&#24863;&#20663;  <pr>&#12363;&#12435;&#12375;&#12423;&#12358;</pr></trd>
  <trdgrp lng="pl">
    <trd>sentymentalno&sacute;&cacute;</trd>,
    <trd>uczuciowo&sacute;&cacute;</trd>
  </trdgrp>
</drv>
</art>
