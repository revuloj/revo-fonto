<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: horor.xml,v 1.10 2015/12/31 16:10:30 revo Exp $">
<kap>
  <rad>horor</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="horor.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="horor.0o.KOMUNE">
    <dif>
      Sento de timego kaj abomeno:
      <ekz>
        Barbara ekridis vidinte mian <tld/>on
        <fnt>
          <aut>S. &Scirc;timec</aut>,
          <vrk>Ombro sur interna pejza&gcirc;o</vrk>,
          <lok>Edistudio, 1984</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="terur.0o">teruro</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>d&ecaron;s</trd>,
    <trd>horor</trd>,
    <trd>pocit hr&uring;zy a odporu</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Horror</trd>,
    <trd>Schrecken</trd>,
    <trd>Entsetzen</trd>,
    <trd>Grauen</trd>
  </trdgrp>
  <trd lng="en">horror</trd>
  <trd lng="fr">horreur</trd>
  <trd lng="it">orrore</trd>
  <trdgrp lng="pl">
    <trd>horror</trd>,
    <trd>zgroza</trd>
  </trdgrp>
  <trd lng="pt">horror</trd>
  <trdgrp lng="sk">
    <trd>des</trd>,
    <trd>hr&ocirc;za</trd>,
    <trd>zdesenie</trd>
  </trdgrp>
</drv>

<drv mrk="horor.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="horor.0a.KOMUNE">
    <dif>
      Kiu ka&ubreve;zas a&ubreve; elmontras <tld/>on:
      <ekz>
        kelkfoje mi kredis a&ubreve;di de fore <tld/>an knaron,
        kvaza&ubreve; de pendumilo en la vento
        <fnt>
          <aut>H. P. Lovecraft, trad. C. Kisa</aut>,
          <vrk>La Soleno</vrk>,
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>hr&uring;zostra&scaron;n&yacute;</trd>,
    <trd>nah&aacute;n&ecaron;j&iacute;c&iacute; hr&uring;zu</trd>
  </trdgrp>
  <trd lng="de">schrecklich</trd>
  <trdgrp lng="en">
    <trd>horrific</trd>,
    <trd>horrible</trd>, <trd>horrendous</trd>
  </trdgrp>
  <trd lng="fr">horrible</trd>
  <trdgrp lng="it">
   <trd>orribile</trd>, 
   <trd>orrendo</trd>, 
   <trd>orrido</trd>, 
   <trd>raccapricciante</trd>
  </trdgrp>
  <trd lng="pl">przera&zdot;aj&aogonek;cy</trd>
  <trdgrp lng="sk">
    <trd>desiv&yacute;</trd>,
    <trd>hrozn&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="horor.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="horor.0i.ntr">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Senti <tld/>on:
      <ekz>
        eble mi sentis min &leftquot;paciento&rightquot;, kiu
        anka&ubreve; en la privata vivo 
        <tld/>is de la kuracisto
        <fnt>
          <aut>F. Szil&aacute;gyi</aut>,
          <vrk><url ref=
"http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr66/zigzage.html">
          Zigzage en Iamujo</url></vrk>, 
          <lok>Nica Literatura Revuo, 1961-07 kaj 08, numero 6/6, p. 
          211a-215a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>d&ecaron;siti se</trd>,
    <trd>poc&iacute;tit hr&uring;zu</trd>
  </trdgrp>
  <trd lng="en">to be horrified</trd>
  <trd lng="fr">&ecirc;tre horrifi&eacute;</trd>
  <trd lng="it">inorridire<klr> (ntr.)</klr></trd>
  <trd lng="pl">by&cacute; przera&zdot;onym</trd>
  <trdgrp lng="sk">
    <trd>zdesi&tcaron; sa</trd>,
    <trd>zhrozi&tcaron; sa</trd>
  </trdgrp>
</drv>

<drv mrk="horor.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="horor.0igi.KOMUNE">
    <dif>
      Ka&ubreve;zi <tld/>on:
      <ekz>
        la ideo eklo&gcirc;i en tia loko <tld/>igis min
        <fnt>
          <aut>A. L&ouml;wenstein</aut>,
          <vrk>La &scirc;tona urbo</vrk>,
          <lok>1999</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>d&ecaron;sit</trd>,
    <trd>nah&aacute;n&ecaron;t hr&uring;zu</trd>
  </trdgrp>
  <trd lng="en">to horrify</trd>
  <trd lng="fr">horrifier</trd>
  <trd lng="it">inorridire<klr> (tr.)</klr></trd>
  <trd lng="pl">przera&zdot;a&cacute;</trd>
  <trdgrp lng="sk">
    <trd>desi&tcaron;</trd>,
    <trd>nah&aacute;&ncaron;a&tcaron; hr&ocirc;zu</trd>
  </trdgrp>
</drv>
</art>

