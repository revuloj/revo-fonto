<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>madon</rad>/o
</kap>
<drv mrk="madon.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      La Dipatrino.
    </dif>
    <trdgrp lng="de">
       <trd>Maria</trd>,
       <trd>Heilige Jungfrau</trd>,
       <trd>Heilige Mutter</trd>,
       <trd>Muttergottes</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Bildo de la Dipatrino.
    </dif>
    <trdgrp lng="de">
       <trd>Marienfigur</trd>,
       <trd>Bildnis der Heiligen Jungfrau</trd>
    </trdgrp>
  </snc>
  <trd lng="de">Madonna</trd>
  <trd lng="hu">madonna</trd>
  <trd lng="nl">Madonna</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_d;&c_o;&c_n;&c_n;&c_a;</trd>,
    <trd>&c_b;&c_o;&c_g;&c_o;&c_m;&c_a;&c_t;&c_je;&c_r;&c_mol;</trd>,
    <trd>&c_b;&c_o;&c_g;&c_o;&c_r;&c_o;&c_d;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
</drv>
</art>
