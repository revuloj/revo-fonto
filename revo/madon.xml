<vortaro>
  <art mrk="$Id: madon.xml,v 1.19 2024/01/25 20:10:33 revo Exp $">
    <kap>
      <ofc>1</ofc>
      <rad>madon</rad>/o
    </kap>

    <drv mrk="madon.0o">
      <kap>
        <tld/>o
      </kap>
      <snc mrk="madon.0o.KAT">
        <uzo tip="fak">KAT</uzo>
          <ref tip="dif" cel="di.0patrino">la Dipatrino</ref>
        <trdgrp lng="de">
          <trd>Maria</trd>,
          <trd>Heilige <ind>Jungfrau</ind></trd>,
          <trd>Heilige <ind>Mutter</ind></trd>,
          <trd>Mutter <ind>Gottes</ind></trd>,
          <trd>Muttergottes</trd>
        </trdgrp>
        <trdgrp lng="pl">
          <trd><klr>&sacute;wi&eogonek;ta </klr>Madonna</trd>,
          <trd>Matka Boska</trd>,
          <trd>Bozia<klr> (dziec.)</klr></trd>
        </trdgrp>
        <trdgrp lng="pt">
          <trd>madona <klr>(rel.)</klr></trd>,
          <trd>Virgem Maria</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_b;&c_o;&c_g;&c_o;&c_m;&c_a;&c_t;&c_je;&c_r;&c_mol;</trd>,
          <trd>&c_b;&c_o;&c_g;&c_o;&c_r;&c_o;&c_d;&c_i;&c_c;&c_a;</trd>
        </trdgrp>
      </snc>
      <snc mrk="madon.0o.BELA">
        <uzo tip="fak">BELA</uzo>
       <dif>
          Okcidenta bildo a&ubreve; skulptita figuro prezentanta la
          Dipatrinon:
          <ekz>
            post pli longa tempo tiuj eternaj <tld/>oj sufi&ccirc;ege
            tedis al mi
            <fnt><bib>Fab4</bib>, <lok>&leftquot;Bela&rightquot;</lok></fnt>.
          </ekz>
        </dif>
        <trdgrp lng="de">
          <trd>Marienfigur</trd>,
          <trd>Bildnis der Heiligen <ind>Jungfrau</ind></trd>
        </trdgrp>
        <trd lng="ru">&c_m;&c_a;&c_d;&c_o;&c_n;&c_n;&c_a;</trd>
      </snc>
      <trdgrp lng="be">
        <trd>&c_m;&c_a;&c_d;&c_o;&c_n;&c_a;</trd>,
        <trd>&c_b;&c_a;&c_g;&c_a;&c_r;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_a;</trd>,
        <trd>&c_m;&c_a;&c_c;&c_ib; &c_b;&c_o;&c_zh;&c_a;&c_ja;</trd>
      </trdgrp>
      <trd lng="br">Itron Varia</trd>
      <trd lng="ca">Mare de D&eacute;u</trd>
      <trd lng="cs">Madona</trd>
      <trd lng="en">Madonna</trd>
      <trd lng="de">Madonna</trd>
      <trd lng="hu">madonna</trd>
      <trd lng="nl">Madonna</trd>
      <trdgrp lng="pl">
        <trd>Madonna</trd>,
        <trd>Matka Boska</trd>,
        <trd>Bozia<klr> (dziec.)</klr></trd>
      </trdgrp>
      <trdgrp lng="zh">
    <trd>&#x5723;&#x6BCD;&#x50CF; <pr>sh&#xE8;ngm&#x1D4;xi&#xE0;ng</pr></trd>,
    <trd>&#x8056;&#x6BCD;&#x50CF; <pr>sh&#xE8;ngm&#x1D4;xi&#xE0;ng</pr></trd>,
    <trd>&#x5723;&#x6BCD;&#x50CF; <pr>sh&#x11B;ngm&#x1D4;xi&#xE0;ng</pr></trd>,
    <trd>&#x8056;&#x6BCD;&#x50CF; <pr>sh&#x11B;ngm&#x1D4;xi&#xE0;ng</pr></trd>,
    <trd>&#x5723;&#x6BCD;&#x739B;&#x5229;&#x4E9A; <pr>sh&#xE8;ngm&#x1D4;m&#x1CE;l&#xEC;y&#xE0;</pr></trd>,
    <trd>&#x8056;&#x6BCD;&#x746A;&#x5229;&#x4E9E; <pr>sh&#xE8;ngm&#x1D4;m&#x1CE;l&#xEC;y&#xE0;</pr></trd>
  </trdgrp>
</drv>
