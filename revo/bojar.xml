<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>bojar</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="bojar.0o">
  <kap><tld/>o</kap>
  <snc mrk="bojar.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Iama rusa a&ubreve; oriente&ubreve;ropa <ref tip="super" 
      cel="nobel.0o.POL">nobelo</ref> altaranga:
      <ekz>
        Apud tablo &scirc;i sidas impone. /
        &Scirc;in priservas nobeloj, <tld/>oj, /
        &Scirc;in regalas per vino transmara
        <fnt>
          <aut>A. Pu&scirc;kin, trad. S. Rublov</aut>,
          <vrk><url
          ref="http://www.russievirtuelle.com/mythologie/contesp/poissondor3.htm">
          La fabelo pri fi&scirc;o kaj pri fi&scirc;isto</url></vrk>,
          <lok>[2008]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">bojar</trd>
  <trd lng="en">boyar</trd>
  <trd lng="fr">boyard</trd>
  <trd lng="sk">bojar</trd>
</drv>
</art>

