<drv mrk="gxu.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="gxu.0i.senti">
    <dif>
      <ref tip="super" cel="sent.0i">Senti</ref> 
      <ref tip="super" cel="plezur.0i">plezur</ref>egon, precipe sentuman:
      <ekz>
        la policisto sidi&gcirc;is al tablo kaj <tld/>is kun granda apetito man&gcirc;ojn kaj
        trinkojn
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        miaj unuaj vortoj estas sincera danko por la gastameco, kiun ni <tld/>is
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado en la Guildhall (Londono) en la 21a de a&ubreve;gusto 1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi melkos la bovinon kaj <tld/>os la fre&scirc;an lakton
        <fnt>
          <bib>ElektFab</bib>
          <lok>Feli&ccirc;a Jo&ccirc;jo</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam arboj plantitaj de mi ekhavos fruktojn <klr>[&#x2026;]</klr> mi komencos <tld/>i
        la vivon
        <fnt>
          <bib>ElektFab</bib>
          <lok>La vivolongo</lok>
        </fnt>;
      </ekz>
      <ekz>
        venu, ni <tld/>u sufi&ccirc;e volupton &gcirc;is la mateno, ni plezuri&gcirc;u per la amo
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 7:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i malpermesitajn plezuregojn en senbrida libereco
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi intencas nun <tld/>i la vesperon, sinjoro
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        nun estas la horo, en kiu mi <tld/>as fre&scirc;an aeron
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>is la societon de bonaj, gracianimaj kaj alte kleraj homoj
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frand.0i">frandi</ref>,
      <ref cel="gxoj.0i">&gcirc;oji</ref>
    </refgrp>
  </snc>
  <snc mrk="gxu.0i.posedi">
    <dif>
      <ref tip="super" cel="posed.0i">Posedi</ref> aferon, de kiu oni havas utilon a&ubreve; plezuron:
      <ekz>
        la vegetarano <tld/>as pli bonan sanon
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la firmo N. <klr>[&#x2026;]</klr> <tld/>as tie &ccirc;i la plej bonan reputacion
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> tuj irus inter la diojn <tld/>i la eternan vivon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ri&ccirc;eco, kiun ni <tld/>as, estas ri&ccirc;eco &scirc;telita
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is la ri&ccirc;econ kaj povon <klr>[&#x2026;]</klr> li ne forpu&scirc;is honorojn, se li
        renkontis ilin
        <fnt>
        </fnt>;
      </ekz>
      <ekz>
        li ne <tld/>is <klr>(<ref tip="super" cel="hav.0i.rilaton">havi</ref>s)</klr> 
        e&ccirc; unu trankvilan tagon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ha&ccirc;jo kaj Grenjo</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu venas plej frue, sidas plej <tld/>e
        <klr>(komforte)</klr><fnt><bib>PrV</bib></fnt>.
        <trd lng="de">fr&uuml;hes Erscheinen sichert die besten Pl&auml;tze</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="profit.0i">profiti</ref>,
      <ref cel="spert.0i">sperti</ref>,
      <ref cel="uz.0i">uzi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_je;&c_sh;&c_y;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_a;&c_t;&c_r;&c_y;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;
    <ind>&c_a;&c_s;&c_a;&c_l;&c_o;&c_d;&c_u;</ind>
    <klr>(&c_z;
    &c_ch;&c_a;&c_g;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  </trdgrp>
  <trd lng="bo">&t_sa;&t_kata;&t_yata;&t_gigu;&t_da;&t_tsheg;&t_pa;&t_naro;&t_tsheg;&t_ga;&t_ta;&t_naro;&t_nga;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>po&zcaron;&iacute;vat</trd>,
    <trd>vychutn&aacute;vat</trd>
  </trdgrp>
  <trd lng="de">genie&szlig;en</trd>
  <trd lng="en">enjoy</trd>
  <trdgrp lng="es">
    <trd>disfrutar</trd>,
    <trd>gozar</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>gaudir</trd>,
    <trd>fruir</trd>
  </trdgrp>
  <trd lng="hu">&eacute;lvez</trd>
  <trdgrp lng="it">
    <trd>godere</trd>, 
    <trd>usufruire</trd>,
    <trd>gustare<klr> (assaporare con piacere)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x697D;&#x3057;&#x3080;  <pr>&#x305F;&#x306E;&#x3057;&#x3080;</pr></trd>,
    <trd>&#x4EAB;&#x53D7;&#x3059;&#x308B;  <pr>&#x304D;&#x3087;&#x3046;&#x3058;&#x3085;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">genieten van</trd>
  <trdgrp lng="pt">
    <trd>gozar</trd>,
    <trd>desfrutar</trd>
  </trdgrp>
  <trd lng="fr">jouir (de)</trd>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_l;&c_a;&c_zh;&c_d;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="sv">njuta</trd>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_a;&c_s;&#x43E;&#x43B;&#x43E;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&#x43A;&#x43E;&c_r;&c_i;&c_s;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F; &c_ch;&c_i;&#x43C;-&#x43D;. &c_ib;&c_z;
    &c_z;&c_a;&c_d;&#x43E;&c_v;&#x43E;&#x43B;&c_je;&#x43D;&#x43D;&#x44F;&#x43C;</trd>,
    <trd>&c_t;&c_ib;&c_sh;&c_i;&c_t;&c_i;&c_s;&#x44F;</trd>
  </trdgrp>
  <trd lng="zh">&#x4EAB;&#x53D7; <pr>xi&#x1CE;ngsh&ograve;u</pr></trd>
</drv>

<drv mrk="gxu.0o">
  <kap><tld/>o, <var><kap><tld/>ado</kap></var></kap>
  <snc mrk="gxu.0o.plezuro">
    <dif>
      Granda senta <ref tip="super" cel="plezur.0o">plezuro</ref> pro io:
      <ekz>
        troa <tld/>ado de alkoholaj trinka&jcirc;oj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la domo estis <ind><tld/>o por la okuloj</ind>
        <fnt><bib>Fab2</bib>, <lok>Malnova domo</lok></fnt>;
        <trd lng="de">Augenweide</trd>
        <trd lng="en">eye candy</trd>
      </ekz>
      <ekz>
        la naturo sin montras &ccirc;i tie en sia tuta grandeco, <klr>[&#x2026;]</klr> la okuloj kaj
        pensoj havas <tld/>on
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        la najtingalo <klr>[&#x2026;]</klr> ekkantis tiel, ke estis plej agrabla <tld/>o &gcirc;in
        a&ubreve;di
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> respondis <klr>[&#x2026;]</klr> en plena <tld/>ado de la pipo, en la
        stato kiam la fumanto povas malfermi la bu&scirc;on nur tre malmulte por paroli
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        odoro, <tld/>o de momento
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        timo miksita kun <tld/>o
        <fnt>
          <bib>FdO</bib>
        </fnt>;
      </ekz>
      <ekz>
        flugis for la &gcirc;ojo de l' juneco, la <tld/>ado de
        la unuaj jaroj
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu amas <tld/>on, amu anka&ubreve; enuon
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        de superfluo malboni&gcirc;as la <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxarm.0o">&ccirc;armo</ref>,
      <ref cel="rav">ravi&gcirc;o</ref>,
      <ref cel="volupt.0o">volupto</ref>.
    </refgrp>
  </snc>
  <snc mrk="gxu.0o.posedo">
    <dif>
      <ref tip="super" cel="posed.0o.eblo">Posedo</ref> de io, de kio oni havas utilon a&ubreve; plezuron:
      <ekz>
        Francujo, laca pro la sanga tragedio <klr>[&#x2026;]</klr>, retrovis rilatan kvietecon
        <klr>[&#x2026;]</klr> dank&#x2019; al <tld/>adoj de lukso
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ekkomprenis, ke ekzistas iu mondo, kie e&ccirc; en sufero estas pli da feli&ccirc;o, ol
        en &ccirc;iuj luksoj kaj <tld/>oj de la cezara domo
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        li vivas en <tld/>o kaj bruo
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_a;&c_l;&c_o;&c_d;&c_a;</trd>
  <trd lng="de">Genuss</trd>
  <trdgrp lng="en">
    <trd>enjoyment</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>gozo</trd>,
    <trd>goce</trd>,
    <trd>disfrute</trd>
  </trdgrp>
  <trd lng="fr">jouissance</trd>
  <trdgrp lng="it">
   <trd>godimeto</trd>, 
   <trd>fruizione</trd>, 
   <trd>goduria</trd> 
  </trdgrp>
  <trd lng="hu">&eacute;lvezet</trd>
  <trd lng="ja">&#x697D;&#x3057;&#x307F;  <pr>&#x305F;&#x306E;&#x3057;&#x307F;</pr></trd>
  <trd lng="nl">genot</trd>
  <trdgrp lng="pt">
    <trd>gozo</trd>, 
    <trd>desfrute</trd> 
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_l;&c_a;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sv">njutning</trd>
  <trd lng="uk">&#x43D;&c_a;&c_s;&#x43E;&#x43B;&#x43E;&c_d;&c_zh;&c_u;&c_v;&c_a;&#x43D;&#x43D;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x4EAB;&#x7528; <pr>xi&#x1CE;ngy&#xF2;ng</pr></trd>,
    <trd>&#x4EAB;&#x7528; <pr>xi&#x1CE;ngy&#xF2;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="gxu.0inda">
  <kap><tld/>inda</kap>
  <snc mrk="gxu.0inda.KOMUNE">
    <dif>
      De kiu oni ricevas atendeblan kaj grandan plezuron:
      <ekz>
        <tld/>indaj ekskursoj
        <fnt>
          <aut>M. Cohen</aut>,
          <vrk><url ref="http://bulteno.esperanto-usa.org/2014/3/40-mekaro.html">
          Raporto de la Mez-Kanada Renkonti&gcirc;o</url></vrk>,
          <lok>Usona Esperantisto, 2014:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        spektaklo, <tld/>inda sed anka&ubreve; ridinda
        <fnt>
          <aut>Jean-Louis Madzella</aut>
          <vrk><url ref="&Monato;/2006/009001.php">&Gcirc;emeloj kaj goriloj</url></vrk>
          <bib>Monato</bib>
          <lok>2006/08, p. 15</lok>
        </fnt>;
      </ekz>
      <ekz>
        la melodioj kaj muzika aran&gcirc;o estas vere <tld/>indaj kaj la tekstoj belsonaj kaj
        pensigaj
        <fnt>
          <aut>Alexander Shlafer</aut>
          <vrk><url ref="&Monato;/2010/009749.php">Plia sukceso de Jacques
          Yvart</url></vrk>
          <bib>Monato</bib>
          <lok>2010/05, p. 20</lok>
        </fnt>;
      </ekz>
      <ekz>
        estis <tld/>inda tio, ke &scirc;i kapablis pri subtile humura sinesprimado
        <fnt>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="&Monato;/2006/008978.php">Mortis Izabel Santiago</url></vrk>
              <bib>Monato</bib>
          <lok>2006/07, p. 17</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>genussvoll</trd>,
    <trd>lustvoll</trd>
  </trdgrp>
  <trd lng="fr">exquis</trd>
</drv>

<drv mrk="gxu.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="gxu.0igi.KOMUNE">
    <dif>
      Provizi <tld/>on:
      <ekz>
        &scirc;iaj karesoj <tld/>igu vin en &ccirc;iu tempo
        <fnt><bib>MT</bib>, <lok>&Sen; 5:19</lok></fnt>;
      </ekz>
      <ekz>
        kio okazos, se li <tld/>igos al la cezaro sprita&jcirc;on
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; unu konstrua&jcirc;on mi ne vidis, pri kiu mi povus konsenti, ke &gcirc;i arte <tld/>igis
        miajn okulojn
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        ironie bonintenca poeto, kiu <tld/>ige pikas per le&gcirc;ertonaj versoj.
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        li sentis sian povon super mi, kaj tio lin <tld/>igis
        <fnt>
          <bib>CKv</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="plezur.0igi">plezurigi</ref>
  </snc>
  <trd lng="be">&c_c;&c_je;&c_sh;&c_y;&c_c;&c_mol;</trd>
  <trdgrp lng="es">
    <trd>hacer disfruta</trd>,
    <trd>hacer gozar</trd>
  </trdgrp>
  <trd lng="fr">faire <ind>profiter</ind> de</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&c_i;&#x43D;&#x43E;&c_s;&c_i;&c_t;&c_i; &#x43D;&c_a;&c_s;&#x43E;&#x43B;&#x43E;&c_d;&c_u;</trd>,
    <trd>&c_z;&c_a;&c_d;&#x43E;&c_v;&#x43E;&#x43B;&c_je;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_v;&c_t;&c_ib;&c_h;&c_u;</trd>
  </trdgrp>
</drv>

