<drv mrk="gxu.0emulo">
  <kap>
    <tld/>emulo
  </kap>
  <snc>
    <dif>
      Iu kiu inklinas al <tld/>o:
      <ekz>
        vi, <tld/>emuloj, miaj fratoj, kiuj estas emaj al &ccirc;io eminda, vino ru&gcirc;a kaj
        blanka, virinoj blondaj kaj brunaj, fre&scirc;a biero, spicitaj man&gcirc;oj kaj
        sprita&jcirc;oj, aromo de brando kaj de printempa vespero, admiro al levi&gcirc;o de la
        suno kaj de jupoj
        <fnt>
          <bib>KrB</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
       <ref cel="viv.bon0anto">bonvivanto</ref>,
       <ref cel="epikur.e0ano.KOMUNE">epikurano<sncref/></ref>,
       <ref cel="hedonist.0o">hedonisto</ref>,
       <ref cel="sibari.0o">sibarito</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Genie&szlig;er</trd>,
    <trd>Genussmensch</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8001;&#x9955; <pr>l&#x1CE;ot&#x101;o</pr></trd>,
    <trd>&#x4EAB;&#x4E50;&#x4E3B;&#x4E49;&#x8005; <pr>xi&#x1CE;ngl&#xE8;zh&#x1D4;y&#xEC;zh&#x11B;</pr></trd>
  </trdgrp>
</drv>


<drv mrk="gxu.antaux0i">
  <kap>anta&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="gxu.antaux0i.KOMUNE">
    <dif>
      <tld lit="&Gcirc;"/>i anticipe, plezure atendi:
      <ekz>
        humile, iom nervoze, mi tamen anta&ubreve;<tld/>as sukcesan kaj fruktodonan kunlaboron
        <fnt>
