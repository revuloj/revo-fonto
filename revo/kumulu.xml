<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kumulu.xml,v 1.25 2023/12/03 09:59:03 revo Exp $">
<kap>
  <rad>kumulus</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kumulu.0o">
  <kap><tld/>o</kap>
  <snc mrk="kumulu.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      blanka, supre pufa <ref tip="super" cel="nub.0o.MET">nubo</ref>,
      ordinare amase aperanta:
      <ekz>
        preteriris anta&ubreve; li diverskoloraj bildoj,
        kelkaj pli klaraj, aliaj malklaraj, preterrapidantaj
        kvaza&ubreve; alt<tld/>oj
        <fnt>
          <aut>J. Linnankoski, trad. V. Set&auml;l&auml;</aut>,
          <vrk>Batalo pri la Domo Heikkil&auml;</vrk>,
          <lok>1919</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_u;&c_b;&c_a;&c_v;&c_a;&c_t;&c_y; &c_v;&c_o;&c_b;&c_l;&c_a;&c_k;</trd>
  <trd lng="ca">c&uacute;mul</trd>
  <trdgrp lng="cs">
    <trd>kumulus</trd>,
    <trd>kupa</trd>,
    <trd>kupovit&yacute; oblak</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kumuluswolke</trd>,
    <trd>Haufenwolke</trd>,
    <trd>Quellwolke</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cumulus</trd>,
    <trd>woolpack</trd>
  </trdgrp>
  <trd lng="es">c&uacute;mulo</trd>
  <trd lng="fa">&f_k;&a_w;&a_m;&a_w;&a_l;&a_w;&a_s;</trd>
  <trd lng="fr">cumulus<klr> (nuage)</klr></trd>
  <trdgrp lng="hu">
    <trd>gomolyfelh&odblac;</trd>,
    <trd>kumulusz</trd>
  </trdgrp>
  <trd lng="ja">&#x7A4D;&#x96F2;  <pr>&#x305B;&#x304D;&#x3046;&#x3093;</pr></trd>
  <trdgrp lng="nl">
    <trd>cumulus</trd>,
    <trd>stapelwolk</trd>
  </trdgrp>
  <trdgrp lng="pl">
  <trd>chmura k&lstroke;&eogonek;biasta</trd>,
  <trd>cumulus <klr>(Cu)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>c&uacute;mulo <klr tip="amb">(meteor.)</klr></trd>,
    <trd>c&uacute;mulos <klr tip="amb">(meteor.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_u;&c_ch;&c_je;&c_v;&c_o;&c_je; &c_o;&c_b;&c_l;&c_a;&c_k;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>kopovit&yacute; oblak</trd>,
    <trd>kumulus</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>cumulusmoln</trd>,
    <trd>stackmoln</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&#x443;&#x43F;&#x447;&#x430;&#x441;&#x442;&#x456; &#x445;&#x43C;&#x430;&#x440;&#x438;</trd>
  <trdgrp lng="zh">
    <trd>&#x79EF;&#x4E91; <pr>j&#x12B;y&#xFA;n</pr></trd>,
    <trd>&#x7A4D;&#x96F2; <pr>j&#x12B;y&#xFA;n</pr></trd>
  </trdgrp>
</drv>
</art>
