<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kumulus</rad>/o
</kap>
<drv mrk="kumulu.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      Aro da amasigitaj blankaj kaj dikaj nuboj.
    </dif>
    <refgrp tip="vid">
      <ref cel="nimbus.0o">nimbuso</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="en">
    <trd>cumulus</trd>,
    <trd>woolpack</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>cumulus</trd>,
    <trd>stapelwolk</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_u;&c_ch;&c_je;&c_v;&c_o;&c_je; &c_o;&c_b;&c_l;&c_a;&c_k;&c_o;</trd>
</drv>
</art>
