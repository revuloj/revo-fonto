<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kumulus</rad>/o
</kap>
<drv mrk="kumulu.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      blanka, supre pufa <ref tip="super" cel="nub.0o.MET">nubo</ref>,
      ordinare amase aperanta.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Kumuluswolke</trd>,
    <trd>Haufenwolke</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cumulus</trd>,
    <trd>woolpack</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cumulus<klr> (nuage)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>cumulus</trd>,
    <trd>stapelwolk</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_u;&c_ch;&c_je;&c_v;&c_o;&c_je; &c_o;&c_b;&c_l;&c_a;&c_k;&c_o;</trd>
</drv>
</art>
