<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>kumulus</rad>/o
</kap>
<drv mrk="kumulu.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      blanka, supre pufa <ref tip="super" cel="nub.0o.MET">nubo</ref>,
      ordinare amase aperanta.
    </dif>
  </snc>
  <trd lng="ca">c&uacute;mul</trd>
  <trdgrp lng="de">
    <trd>Kumuluswolke</trd>,
    <trd>Haufenwolke</trd>,
    <trd>Quellwolke</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cumulus</trd>,
    <trd>woolpack</trd>
  </trdgrp>
  <trd lng="es">c&uacute;mulo</trd>
  <trdgrp lng="fr">
    <trd>cumulus<klr> (nuage)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gomolyfelh&odblac;</trd>,
    <trd>kumulusz</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>cumulus</trd>,
    <trd>stapelwolk</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>c&uacute;mulo <klr tip="amb">(meteor.)</klr></trd>,
    <trd>c&uacute;mulos <klr tip="amb">(meteor.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_u;&c_ch;&c_je;&c_v;&c_o;&c_je;
  &c_o;&c_b;&c_l;&c_a;&c_k;&c_o;</trd>
  <trdgrp lng="sv">
    <trd>cumulusmoln</trd>,
    <trd>stackmoln</trd>
  </trdgrp>
</drv>
</art>
