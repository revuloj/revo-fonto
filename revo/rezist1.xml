<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rezist1.xml,v 1.19 2020/02/16 12:10:37 revo Exp $">
<kap>
  <rad>rezistanc</rad>/o
</kap>
<drv mrk="rezist1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="grand.fizika0o">Fizika grando</ref>
      karakterizanta kondukilon, dependa &ccirc;efe de &gcirc;ia naturo,
      longo kaj tratran&ccirc;o, difinita per la le&gcirc;o de Omo, kaj
      egala al rilato inter
      <ref tip="vid" cel="tensi.0o.ELE">tensio</ref>
      kaj <ref tip="vid" cel="kurent.0intenso">kurentintenso</ref>:
      <ekz>
        la <ref cel="dimens.0o.FIZ">dimensioj</ref> de <tld/>o estas
    <frm am="M^1L^2T^-3I^-2"
    >M<sup>1</sup>L<sup>2</sup>T<sup>-3</sup>I<sup>-2</sup></frm>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konduk1.0o">konduktanco</ref>,
      <ref cel="om.o0o">omo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_p;&c_r;&c_a;&c_c;&c_ib;&c_w;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;
  <klr>(&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_je;)
  </klr>
  </trd>
  <trd lng="ca">resist&egrave;ncia</trd>
  <trdgrp lng="cs">
    <trd>rezistance</trd>,
    <trd>&ccaron;inn&yacute; odpor</trd>
  </trdgrp>
  <trd lng="de">Widerstand</trd>
  <trd lng="fa">&a_m;&a_q;&a_A;&a_w;&a_m;&a_t;<klr tip="ind"> (&a_b;&a_r;&a_q;)</klr></trd>
  <trd lng="fr">r&eacute;sistance<klr tip="ind"> (&eacute;lec.)</klr>
  </trd>
  <trd lng="hu">ellen&aacute;ll&aacute;s</trd>
  <trd lng="nl">weerstand<klr tip="ind"> (elektr.)</klr></trd>
  <trdgrp lng="pl">
    <trd>rezystancja</trd>,
    <trd>oporno&sacute;&cacute;<klr> elektryczna</klr></trd>
  </trdgrp>
  <trd lng="pt">resist&ecirc;ncia</trd>
  <trd lng="ru">&c_s;&c_o;&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je; <klr
  tip="amb">(&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;.)</klr>
  </trd>
  <trd lng="sk">&ccaron;inn&yacute; odpor</trd>
  <trd lng="sv">resistans</trd>
</drv>
</art>
