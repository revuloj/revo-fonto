<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>rezistanc</rad>/o
</kap>
<drv mrk="rezist1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="grand.fizika0o">Fizika grando</ref>
      karakterizanta kondukilon, dependa &ccirc;efe de &gcirc;ia naturo,
      longo kaj tratran&ccirc;o, difinita per la le&gcirc;o de Omo, kaj
      egala al rilato inter <ref tip="vid" cel="tensi.0o.ELE">tensio</ref>
      kaj <ref tip="vid" cel="kurent.0intenso">kurentintenso</ref>:
      <ekz>
        la <ref cel="dimens.0o.FIZ">dimensioj</ref> de <tld/>o estas
        <frm>M<sup>1</sup>L<sup>2</sup>T<sup>-3</sup>I<sup>-2</sup></frm>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konduk1.0o">konduktanco</ref>,
      <ref cel="om.o0o">omo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_p;&c_r;&c_a;&c_c;&c_ib;&c_w;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je; <klr>(&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_je;)</klr></trd>
  <trd lng="de">Widerstand</trd>
  <trd lng="fr">r&eacute;sistance<klr tip="ind"> (&eacute;lec.)</klr></trd>
  <trd lng="hu">ellen&aacute;ll&aacute;s</trd>  
  <trd lng="nl">weerstand<klr tip="ind"> (elektr.)</klr></trd>
  <trd lng="pt">resist&ecirc;ncia</trd>
  <trd lng="ru">&c_s;&c_o;&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je; <klr tip="amb">(&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;.)</klr></trd>
  <trd lng="sv">resistans</trd>
</drv>
</art>
