<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>seren</rad>/a
</kap>
<drv mrk="seren.0a">
  <kap><tld/>a</kap>
  <snc mrk="seren.0a.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Sennuba kaj senventa:
      <ekz>
        <tld/>a frosta vetero<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hel.0a">hela</ref>
    </refgrp>
    <trd lng="be">&c_ja;&c_s;&c_n;&c_y;</trd>
    <trd lng="de">wolkenlos</trd>
    <trd lng="hu">der&uuml;lt</trd>    
    <trd lng="nl">helder</trd>
    <trd lng="ru">&c_ja;&c_s;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="seren.0a.anime">
    <dif>
      Kvieta sen &ccirc;agrenoj, sen videbla anta&ubreve;timo a&ubreve; stre&ccirc;o:
      <ekz>
        <tld/>a vo&ccirc;o, rideto, diskutado;
      </ekz>
      <ekz>
        lia frunto <tld/>i&gcirc;is<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_ja;&c_s;&c_n;&c_y;</trd>,
      <trd>&c_s;&c_mol;&c_v;&c_je;&c_t;&c_l;&c_y;</trd>
    </trdgrp>
    <trd lng="de">heiter</trd>
    <trd lng="hu">der&udblac;s</trd>    
    <trd lng="nl">sereen</trd>
    <trd lng="pt">sereno</trd>
    <trdgrp lng="ru">
      <trd>&c_ja;&c_s;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_v;&c_je;&c_t;&c_l;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">serein</trd>
</drv>

<drv mrk="seren.0o">
  <kap><tld/>o</kap>
  <snc mrk="seren.0o..MET">
    <dif>
      Senventa, hela vetero:
      <ekz>
        la vento &ccirc;esi&gcirc;is, kaj fari&gcirc;is granda <tld/>o
        <fnt><bib>NT</bib>, <lok>&Mar; 4:39</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="hu">der&uuml;lt id&odblac;</trd>    
  </snc>
  <snc mrk="seren.0o.anima">
    <dif>
      <ref tip="super" cel="kviet.0o.pacaspekto">Kvieto</ref>, videbla
      senzorgeco, paca stato:
      <ekz>
      </ekz>
    </dif>
    <trd lng="hu">der&udblac;</trd>    
  </snc>
  <trd lng="fr">s&eacute;r&eacute;nit&eacute;</trd>
</drv>

<drv mrk="seren.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="seren.mal0a.MET">
    <dif>
      Venta, nuba a&ubreve; iel agita:
      <ekz>sub mal<tld/>a &ccirc;ielo de frua printempo
      <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 19a, p. 190a</lok></fnt>.</ekz>
    </dif>
    <trd lng="hu">bor&uacute;s</trd>    
  </snc>
  <snc mrk="seren.mal0a.anime">
    <dif>
      Malkvieta pro anima stre&ccirc;o:
      <ekz>
        mal<tld/>a rigardo;
      </ekz>
      <ekz>
        mal<tld/>a kolermiena malliberulo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mal<tld/>a domo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mal<tld/>e flamantaj okuloj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>nyomaszt&oacute;</trd>,
      <trd>fesz&uuml;lt</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>inquiet</trd>,
    <trd>perturb&eacute;</trd>
  </trdgrp>
  <trdgrp lng="be">
    <trd>&c_z;&c_m;&c_r;&c_o;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_u;&c_r;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_h;&c_m;&c_u;&c_r;&c_n;&c_y;</trd>,
    <trd>&c_h;&c_m;&c_u;&c_r;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_r;&c_a;&c_ch;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
