<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>seren</rad>/a
</kap>
<drv mrk="seren.0a">
  <kap><tld/>a</kap>
  <snc mrk="seren.0a.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Sennuba kaj senventa:
      <ekz>
        <tld/>a frosta vetero<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hel.0a">hela</ref>
    </refgrp>
    <trd lng="be">&c_ja;&c_s;&c_n;&c_y;</trd>
    <trdgrp lng="de">
      <trd>wolkenlos</trd>,
      <trd>heiter</trd>
    </trdgrp>
    <trd lng="hu">der&uuml;lt</trd>
    <trd lng="nl">helder<klr tip="ind"> (weer)</klr></trd>
  <trdgrp lng="pt">
    <trd>sereno</trd>,
    <trd>calmo</trd>
  </trdgrp>
    <trd lng="ru">&c_ja;&c_s;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="seren.0a.anime">
    <dif>
      Kvieta sen &ccirc;agrenoj, sen videbla anta&ubreve;timo a&ubreve; stre&ccirc;o:
      <ekz>
        <tld/>a vo&ccirc;o, rideto, diskutado;
      </ekz>
      <ekz>
        lia frunto <tld/>i&gcirc;is<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_ja;&c_s;&c_n;&c_y;</trd>,
      <trd>&c_s;&c_mol;&c_v;&c_je;&c_t;&c_l;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>ruhig</trd>,
      <trd>entspannt</trd>
    </trdgrp>
    <trd lng="hu">der&udblac;s</trd>
    <trd lng="nl">sereen</trd>
    <trd lng="pt">sereno</trd>
    <trdgrp lng="ru">
      <trd>&c_ja;&c_s;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_v;&c_je;&c_t;&c_l;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="en">
    <trd>serene</trd>,
    <trd>calm</trd>
  </trdgrp>
  <trd lng="fr">serein</trd>
  <trd lng="tr">huzurlu</trd>
</drv>

<drv mrk="seren.0o">
  <kap><tld/>o</kap>
  <snc mrk="seren.0o..MET">
    <dif>
      Senventa, hela vetero:
      <ekz>
        la vento &ccirc;esi&gcirc;is, kaj fari&gcirc;is granda <tld/>o
        <fnt><bib>NT</bib>, <lok>&Mar; 4:39</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>wolkenloses Wetter</trd>,
      <trd>heiteres Wetter</trd>
    </trdgrp>
    <trd lng="hu">der&uuml;lt id&odblac;</trd>
    <trd lng="nl">helder <ind>weer</ind></trd>
    <trd lng="pt">quietude</trd>
    <trd lng="ru">&c_ja;&c_s;&c_n;&c_a;&c_ja; &c_p;&c_o;&c_g;&c_o;&c_d;&c_a;</trd>
  </snc>
  <snc mrk="seren.0o.anima">
    <dif>
      <ref tip="super" cel="kviet.0o.pacaspekto">Kvieto</ref>, videbla
      senzorgeco, paca stato:
      <ekz>
      </ekz>
    </dif>
    <trd lng="hu">der&udblac;</trd>
  <trdgrp lng="pt">
    <trd>serenidade</trd>,
    <trd>quietude</trd>
  </trdgrp>
  <trd lng="nl">sereniteit</trd>
    <trdgrp lng="ru">
      <trd>&c_b;&c_je;&c_z;&c_m;&c_ja;&c_t;&c_je;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_p;&c_o;&c_k;&c_o;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>,
      <trd>&c_t;&c_i;&c_sh;&c_i;&c_n;&c_a;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="en">
    <trd>serenity</trd>,
    <trd>calmness</trd>
  </trdgrp>
  <trd lng="fr">s&eacute;r&eacute;nit&eacute;</trd>
  <trd lng="tr">huzurluluk</trd>
</drv>

<drv mrk="seren.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="seren.mal0a.MET">
    <dif>
      Venta, nuba a&ubreve; iel agita:
      <ekz>sub mal<tld/>a &ccirc;ielo de frua printempo
      <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 19a, p. 190a</lok></fnt>.</ekz>
    </dif>
    <trd lng="hu">bor&uacute;s</trd>
  <trdgrp lng="pt">
    <trd>umbroso</trd>,
    <trd>agitado</trd>,
    <trd>borrascoso</trd>
  </trdgrp>
  </snc>
  <snc mrk="seren.mal0a.anime">
    <dif>
      Malkvieta pro anima stre&ccirc;o:
      <ekz>
        mal<tld/>a rigardo;
      </ekz>
      <ekz>
        mal<tld/>a kolermiena malliberulo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mal<tld/>a domo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mal<tld/>e flamantaj okuloj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>nyomaszt&oacute;</trd>,
      <trd>fesz&uuml;lt</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>inquieto</trd>,
      <trd>agitado</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_m;&c_r;&c_o;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_u;&c_r;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_h;&c_m;&c_u;&c_r;&c_n;&c_y;</trd>,
    <trd>&c_h;&c_m;&c_u;&c_r;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>stormy</trd>,
    <trd>agitated</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>inquiet</trd>,
    <trd>perturb&eacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>somber</trd>,
    <trd>donker</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_h;&c_m;&c_u;&c_r;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_m;&c_u;&c_r;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_m;&c_r;&c_a;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>huzursuz</trd>,
    <trd>kayg&inodot;l&inodot;</trd>
  </trdgrp>
</drv>
</art>
