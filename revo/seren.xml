<drv mrk="seren.0a">
  <kap><tld/>a</kap>
  <snc mrk="seren.0a.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Sennuba kaj senventa:
      <ekz>
        <tld/>a varmego
        <fnt>
          <bib>MT</bib>
          <lok>&Jes; 18:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a frosta vetero
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne&gcirc;o <klr>[&#x2026;]</klr> brilanta <klr>[&#x2026;]</klr> sub la radioj de la
        suno,
        kiu hele lumis sur la <tld/>a &ccirc;ielo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
         ili dancadis &ccirc;e la senbrua lago en la <tld/>a lunlumo
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hel.0a">hela</ref>
    </refgrp>
    <trd lng="be">&c_ja;&c_s;&c_n;&c_y;</trd>
    <trdgrp lng="de">
      <trd>wolkenlos</trd>,
      <trd>heiter</trd>
    </trdgrp>
    <trd lng="hu">der&uuml;lt</trd>
    <trd lng="nl">helder<klr tip="ind"> (weer)</klr></trd>
  <trdgrp lng="pt">
    <trd>sereno</trd>,
    <trd>calmo</trd>
  </trdgrp>
    <trd lng="ru">&c_ja;&c_s;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="seren.0a.anime">
    <dif>
      Kvieta sen &ccirc;agrenoj, sen videbla anta&ubreve;timo a&ubreve; stre&ccirc;o:
      <ekz>
        <tld/>a vo&ccirc;o, rideto, diskutado;
      </ekz>
      <ekz>
        &scirc;i estis tre bela, brilante <tld/>a, e&ccirc; la okuloj de mia patrino
        ne estis pli mildaj ol &scirc;iaj
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
         la bela viza&gcirc;o ree fari&gcirc;is <tld/>a, e&ccirc; ekradiis de plezuro
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        lia frunto <tld/>i&gcirc;is
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro 5a</lok>
        </fnt>;
      </ekz>
      <ekz>
        ru&gcirc;eto aperis sur la palaj vangoj, la okuloj flamis de <tld/>eco
        kaj entuziasmo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igita de la morto nobla viza&gcirc;o
        <fnt>
          <bib>Fab3</bib>
          <lok>Lasta perlo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_ja;&c_s;&c_n;&c_y;</trd>,
      <trd>&c_s;&c_mol;&c_v;&c_je;&c_t;&c_l;&c_y;</trd>,
      <trd>&c_c;&c_ib;&c_h;&c_a;&c_m;&c_ib;&c_r;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>ruhig</trd>,
      <trd>entspannt</trd>
    </trdgrp>
    <trd lng="hu">der&udblac;s</trd>
    <trdgrp lng="nl">
      <trd>sereen</trd>,
      <trd>onbezorgd</trd>
    </trdgrp>
    <trd lng="pt">sereno</trd>
    <trdgrp lng="ru">
      <trd>&c_ja;&c_s;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_v;&c_je;&c_t;&c_l;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>jasn&yacute;</trd>,
    <trd>klidn&yacute;</trd>,
    <trd>nezkalen&yacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>serene</trd>,
    <trd>calm</trd>
  </trdgrp>
  <trd lng="fr">serein</trd>
  <trdgrp lng="ja">
    <trd>&#x6674;&#x6717;&#x306A;  <pr>&#x305B;&#x3044;&#x308D;&#x3046;&#x306A;</pr></trd>,
    <trd>&#x6674;&#x308C;&#x6674;&#x308C;&#x3057;&#x305F;  <pr>&#x306F;&#x308C;&#x3070;&#x308C;&#x3057;&#x305F;</pr></trd>,
    <trd>&#x304A;&#x3060;&#x3084;&#x304B;&#x306A;</trd>,
    <trd>&#x3046;&#x3089;&#x3089;&#x304B;&#x306A;</trd>,
    <trd>&#x306E;&#x3069;&#x304B;&#x306A;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pogodny</trd>,
    <trd>bezchmurny</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>jasn&yacute;</trd>,
    <trd>neskalen&yacute;</trd>
  </trdgrp>
  <trd lng="tr">huzurlu</trd>
  <trdgrp lng="uk">
    <trd>&c_ja;&c_s;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_v;&c_ib;&c_t;&c_l;&c_i;&c_j;</trd>,
    <trd>&c_t;&c_i;&c_h;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_g;&c_ib;&c_d;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_h;&c_m;&c_a;&c_r;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_t;&c_u;&c_r;&c_b;&c_o;&c_t;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_p;&c_o;&c_k;&c_ib;&c_j;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_z;&c_v;&c_o;&c_r;&c_u;&c_sh;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5B89;&#x8A73; <pr>&#x101;nxi&#xE1;ng</pr></trd>,
    <trd>&#x5B89;&#x8BE6; <pr>&#x101;nxi&#xE1;ng</pr></trd>,
    <trd>&#x5E73;&#x9759; <pr>p&#xED;ngj&#xEC;ng</pr></trd>,
    <trd>&#x5E73;&#x975C; <pr>p&#xED;ngj&#xEC;ng</pr></trd>,
    <trd>&#x6C89;&#x9759; <pr>ch&#xE9;nj&#xEC;ng</pr></trd>,
    <trd>&#x6C89;&#x975C; <pr>ch&#xE9;nj&#xEC;ng</pr></trd>,
    <trd>&#x8A73;&#x548C; <pr>xi&#xE1;ngh&#xE9;</pr></trd>,
    <trd>&#x8BE6;&#x548C; <pr>xi&#xE1;ngh&#xE9;</pr></trd>,
    <trd>&#x9591;&#x975C; <pr>xi&#xE1;nj&#xEC;ng</pr></trd>,
    <trd>&#x95F2;&#x9759; <pr>xi&#xE1;nj&#xEC;ng</pr></trd>,
    <trd>&#x9759; <pr>j&#xEC;ng</pr></trd>,
    <trd>&#x975C; <pr>j&#xEC;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="seren.0o">
  <kap><tld/>o</kap>
  <snc mrk="seren.0o.MET">
    <dif>
      Senventa, hela vetero:
      <ekz>
        la vento &ccirc;esi&gcirc;is, kaj fari&gcirc;is granda <tld/>o
        <fnt><bib>NT</bib> <lok>&Mar; 4:39</lok></fnt>;
      </ekz>
      <ekz>
        la boforta skalo estas skalo por taksi la fortecon (rapidecon)
        de vento <klr>[&#x2026;]</klr> indico 1 &#x2010; sereno &#x2010; 2 &gcirc;is 5 km/h
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=49869">Boforta
          skalo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_ja;&c_s;&c_n;&c_a;&c_je; <ind>&c_n;&c_a;&c_d;&c_v;&c_o;&c_r;&#x2019;&c_je;</ind></trd>
    <trdgrp lng="de">
      <trd>wolkenloses Wetter</trd>,
      <trd>heiteres Wetter</trd>
    </trdgrp>
    <trd lng="hu">der&uuml;lt id&odblac;</trd>
    <trd lng="nl">helder <ind>weer</ind></trd>
    <trd lng="pt">quietude</trd>
    <trd lng="ru">&c_ja;&c_s;&c_n;&c_a;&c_ja;
    &c_p;&c_o;&c_g;&c_o;&c_d;&c_a;</trd>
  </snc>
  <snc mrk="seren.0o.anima">
    <dif>
      <ref tip="super" cel="kviet.0o.paco">Kvieto</ref>, videbla
      senzorgeco, paca stato:
      <ekz>
        la ludo kun la filo &ccirc;iam kvietigis lian &ccirc;agrenon kaj plenigis
        per <tld/>o lian koron
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro 12a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_p;&c_a;&c_k;&c_o;&c_j;</trd>,
      <trd>&c_c;&c_ib;&c_h;&c_a;&c_m;&c_ib;&c_r;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_c;&c_ib;&c_sh;&c_a;</trd>
    </trdgrp>
    <trd lng="cs">&ccaron;&iacute;&zcaron;ek</trd>
    <trd lng="hu">der&udblac;</trd>
  <trdgrp lng="pt">
    <trd>serenidade</trd>,
    <trd>quietude</trd>
  </trdgrp>
  <trd lng="nl">sereniteit</trd>
   <trdgrp lng="ru">
      <trd>&c_b;&c_je;&c_z;&c_m;&c_ja;&c_t;&c_je;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_p;&c_o;&c_k;&c_o;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>,
      <trd>&c_t;&c_i;&c_sh;&c_i;&c_n;&c_a;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="en">
    <trd>serenity</trd>,
    <trd>calmness</trd>
  </trdgrp>
  <trd lng="fr">s&eacute;r&eacute;nit&eacute;</trd>
  <trd lng="pl">pogodno&sacute;&cacute;</trd>
  <trd lng="sk">stehl&iacute;k&nbsp;&ccaron;&iacute;&zcaron;av&yacute;</trd>
  <trd lng="tr">huzurluluk</trd>
  <trdgrp lng="zh">
    <trd>&#x5B81;&#x9759; <pr>n&#xED;ngj&#xEC;ng</pr></trd>,
    <trd>&#x5BE7;&#x975C; <pr>n&#xED;ngj&#xEC;ng</pr></trd>,
    <trd>&#x5E73;&#x5E38;&#x5FC3; <pr>p&#xED;ngch&#xE1;ngx&#x12B;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="seren.0igxi">
  <kap>
    <tld/>i&gcirc;i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="seren.0igxi.MET">
    <dif>
      I&gcirc;i <tld/>a, kvieta:
      <ekz>
        tuj la vetero <tld/>i&gcirc;is kaj ili ekveturis
        <fnt>
          <bib>Fab2</bib>
          <vrk>Dio de dormo</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_a;&c_ja;&c_s;&c_mol;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_s;&c_u;&c_p;&c_a;&c_k;&c_o;&c_j;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="seren.0igxi.anime">
    <dif>
      I&gcirc;i anime <tld/>a, trankvila:
      <ekz>
        <tld/>i&gcirc;is &scirc;ia dol&ccirc;a viza&gcirc;o, kaj la larmoj &ccirc;esis flui
        <fnt>
          <aut>H. Luyken</aut>
          <vrk>Mirinda amo</vrk>
          <lok>1913</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_u;&c_p;&c_a;&c_k;&c_o;&c_j;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_u;&c_t;&c_a;&c_j;&c_m;&c_o;&c_w;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x6674;&#x308C;&#x3084;&#x304B;&#x306B;&#x306A;&#x308B; 
    <pr>&#x306F;&#x308C;&#x3084;&#x304B;&#x306B;&#x306A;&#x308B;</pr></trd>,
    <trd>&#x304A;&#x3060;&#x3084;&#x304B;&#x306B;&#x306A;&#x308B;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przeja&sacute;nia&cacute; si&eogonek;</trd>,
    <trd>wypogadza&cacute; si&eogonek;</trd>
  </trdgrp>
</drv>

<drv mrk="seren.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="seren.mal0a.MET">
    <dif>
      Venta, nuba a&ubreve; iel agitita:
      <ekz>
        pluva mal<tld/>a vespero
        <fnt>
          <bib>Monato</bib>
          <aut>Anna kaj Mati Pentus</aut>
          <vrk><url ref="&Monato;/2011/010229.php">Vagadoj
          urbaj kaj mensaj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        sub mal<tld/>a &ccirc;ielo de frua printempo
        <fnt><bib>MkM</bib> <lok>&ccirc;apitro 19a, p. 190a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_je;&c_p;&c_a;&c_g;&c_o;&c_d;&c_l;&c_ib;&c_v;&c_y;</trd>,
      <trd>&c_n;&c_je;&c_p;&c_a;&c_g;&c_o;&c_d;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="hu">bor&uacute;s</trd>
  <trdgrp lng="nl">
      <trd>betrokken</trd>,
      <trd>bewolkt</trd>
    </trdgrp>
    <trdgrp lng="pt">
    <trd>umbroso</trd>,
    <trd>agitado</trd>,
    <trd>borrascoso</trd>
  </trdgrp>
  </snc>
  <snc mrk="seren.mal0a.anime">
    <dif>
      Malkvieta pro anima stre&ccirc;o:
      <ekz>
        mal<tld/>a rigardo;
      </ekz>
      <ekz>
        mal<tld/>a kolermiena malliberulo
        <fnt>
          <bib>Fab2</bib>
          <lok>Bildo el kastelremparo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>aj, malbonimpresaj domoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Bildo el kastelremparo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>e flamantaj okuloj
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  <trdgrp lng="be">
    <trd>&c_z;&c_m;&c_r;&c_o;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_u;&c_r;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_h;&c_m;&c_u;&c_r;&c_n;&c_y;</trd>,
    <trd>&c_h;&c_m;&c_u;&c_r;&c_n;&c_y;</trd>
  </trdgrp>
    <trdgrp lng="hu">
      <trd>nyomaszt&oacute;</trd>,
      <trd>fesz&uuml;lt</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>inquieto</trd>,
      <trd>agitado</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>matn&yacute;</trd>,
    <trd>mdl&yacute;</trd>,
    <trd>pochmurn&yacute;</trd>,
    <trd>ponur&yacute;</trd>,
    <trd>temn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>aufgew&uuml;hlt</trd>,
    <trd>unruhig</trd>,
    <trd>tr&uuml;bsinnig</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>stormy</trd>,
    <trd>agitated</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>inquiet</trd>,
    <trd>perturb&eacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x304F;&#x3082;&#x3063;&#x305F;</trd>,
    <trd>&#x9670;&#x9B31;&#x306A;  <pr>&#x304B;&#x3052;&#x3046;&#x3064;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>somber</trd>,
    <trd>donker</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>chmurny</trd>,
    <trd>niepogodny</trd>,
    <trd>zas&eogonek;piony</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_h;&c_m;&c_u;&c_r;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_m;&c_u;&c_r;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_m;&c_r;&c_a;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chm&uacute;rny</trd>,
    <trd>pochm&uacute;rny</trd>
