</drv>

<drv mrk="seren.mal0igxi">
  <kap>
    mal<tld/>i&gcirc;i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="seren.mal0igxi.MET">
    <dif>
      I&gcirc;i mal<tld/>a, agitita, malkvieta:
      <ekz>
        la maro mal<tld/>i&gcirc;is de forta vento
        <fnt>
          <bib>NT</bib>
          <lok>&Joh; 6:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;ielo pli kaj pli mal<tld/>i&gcirc;is, ekkoleris
        <fnt>
          <aut>I. Bukurauli, trad. E. Oganezov</aut>
          <vrk>La morto de &scirc;afisto</vrk>
          <lok>en: Orienta Almanako, 1913</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_h;&c_m;&c_u;&c_r;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_n;&c_a;&c_h;&c_m;&c_a;&c_r;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_h;&c_v;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="seren.mal0igxi.anime">
    <dif>
      I&gcirc;i malkvieta, <ref tip="vid" cel="pertur.0igxi">perturbi&gcirc;i</ref>:
      <ekz>
        &ccirc;e tio la okuloj de Ursus mal<tld/>i&gcirc;is, kaj post momento li aldonis: &ndash; Se mi
        &scirc;in ne obeus, vi jam ne vivus, sinjoro
        <fnt>
          <aut>H: Sienkiewicz, trad. Lidja Zamenhof</aut>
          <vrk>Quo Vadis</vrk>
