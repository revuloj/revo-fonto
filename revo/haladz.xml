<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>haladz</rad>/o
</kap>
<drv mrk="haladz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Malbonodora, pli malpli venena 
      <ref tip="super" cel="gas.0o">gaso</ref>,
      eli&gcirc;anta el korpo a&ubreve;
      objekto; <ref tip="vid" cel="miasm.0o">miasmo</ref>:
      <ekz>
        mortiga, na&ubreve;za, sufoka <tld/>o;
        mar&ccirc;a, strata <tld/>o;
      </ekz>
      <ekz>
        restos nur fumo, vaporo, haladzo kaj cindro
        <fnt>(<aut>Andersen, trad. Zamenhof</aut>: <vrk>Fabeloj,
        La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</vrk>)</fnt>
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="fum.0o">fumo</ref>,
    <ref cel="vapor.0o">vaporo</ref>,
  </refgrp>
  <trd lng="de">Ausd&uuml;nstung</trd>
  <trd lng="en">fume</trd>
</drv>
<drv mrk="haladz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Eligi <tld/>on
    </dif>
    <refgrp tip="sin">
      <ref cel="spir.el0i">elspiri</ref>,
      <ref cel="sxvit.0i">&scirc;viti</ref>.
    </refgrp>
  </snc>
  <trd lng="de">ausd&uuml;nsten</trd>
</drv>
<drv mrk="haladz.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Malbonodora, na&ubreve;za, putra:
      <ekz>
        <tld/>aj vaporoj de sulfuro;
      </ekz>
      <ekz>
        malluma <tld/>a kafejo<fnt>B</fnt>;
      </ekz>
      <ekz>
        venenaj mu&scirc;oj putrigas kaj <tld/>igas la oleon de
        parfumisto
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Predikanto 10:1</lok>)</fnt>.
      </ekz>
    </dif>
  </snc>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
