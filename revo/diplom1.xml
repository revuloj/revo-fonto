<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: diplom1.xml,v 1.23 2023/11/04 14:29:02 revo Exp $">
<kap>
  <ofc>*</ofc><ofc>1</ofc><ofc>7</ofc>
  <rad>diplomat</rad>/o
</kap>
<drv mrk="diplom1.0o">
  <kap><tld/>o</kap>
  <snc mrk="diplom1.0o.POL">
  <uzo tip="fak">POL</uzo>
    <dif>
      Specialisto pri <ref cel="diplom2.0o.arto">diplomatio
      <sncref/></ref>:
      <ekz>
        aferoj speciale politikaj <klr>[&#x2026;]</klr> apartenas al la <tld/>oj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Tria Kongreso Esperantista en Cambridge en la 12a de a&ubreve;gusto 1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Musavasa</nom>, kvankam ruza militestro kaj <tld/>o, lasis sin trompi
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        dek kvar principoj noblaj de <nom>Wilson</nom> nur montris al regantaj <tld/>oj tion, kion
        ili devus fari
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        kontra&ubreve; du samtempaj ofensivoj, el kiu unu trudis ur&gcirc;an priatenton, e&ccirc; plej
        sperta <tld/>o ne povas venki
        <fnt>
          <bib>CKv</bib>
        </fnt>;
      </ekz>
      <ekz>
        la francaj <tld/>oj devos lerni la germanan
        lingvon
        <fnt><aut>Stefan Maul</aut>: <vrk>Lingvopakto</vrk>,
        <bib>Monato</bib><lok>jaro 2000a, numero 12a, p. 7a</lok></fnt>.
      </ekz>
    </dif>
    <rim>
      <ekz>
        <ref tip="sin" cel="diplom2.0isto">Diplomatiisto</ref>n oni povas
        anka&ubreve; nomi <tld/>o, sed fizikiston oni ne povas nomi
        fiziko, &ccirc;ar fiziko estas la nomo de la scienco mem
        <fnt>
          <bib>F</bib>
          <lok>&FE; 32</lok>
        </fnt>.
      </ekz>
    </rim>    
    <trd lng="de">Diplomat</trd>
    <trd lng="pl">dyplomata</trd>
    <trdgrp lng="zh">
    <trd>&#x5916;&#x4EA4;&#x5B98; <pr>w&#xE0;iji&#x101;ogu&#x101;n</pr></trd>,
    <trd>&#x5916;&#x4EA4;&#x5BB6; <pr>w&#xE0;iji&#x101;oji&#x101;</pr></trd>
  </trdgrp>
</snc>
<!-- ne trovis tian uzon, kaj se fakte ekzistas eble sufi&ccirc;os kiel ekz-o FIG sub la propra snc 
  <snc mrk="diplom1.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ruzulo, havanta kvaza&ubreve; <tld/>an lertecon.
    </dif>
    <trd lng="de">Bauernf&auml;nger</trd>
  </snc>
-->
  <trd lng="be">&c_d;&c_y;&c_p;&c_l;&c_ja;&c_m;&c_a;&c_t;</trd>
  <trd lng="cs">diplomat</trd>
  <trd lng="es">diplom&aacute;tico</trd>
  <trd lng="fr">diplomate</trd>
  <trdgrp lng="ja">
    <trd>&#x5916;&#x4EA4;&#x5B98;  <pr>&#x304C;&#x3044;&#x3053;&#x3046;&#x304B;&#x3093;</pr></trd>,
    <trd>&#x5916;&#x4EA4;&#x5BB6;  <pr>&#x304C;&#x3044;&#x3053;&#x3046;&#x304B;</pr></trd>,
    <trd>&#x99C6;&#x3051;&#x5F15;&#x304D;&#x306E;&#x3046;&#x307E;&#x3044;&#x4EBA;  <pr>&#x304B;&#x3051;&#x3072;&#x304D;&#x306E;&#x3046;&#x307E;&#x3044;&#x3058;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="pt">diplomata</trd>
  <trd lng="ru">&c_d;&c_i;&c_p;&c_l;&c_o;&c_m;&c_a;&c_t;</trd>
  <trd lng="sk">diplomat</trd>
  <trd lng="uk">&#x434;&#x438;&#x43F;&#x43B;&#x43E;&#x43C;&#x430;&#x442;</trd>
</drv>

<drv mrk="diplom1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      De <tld/>o:
      <ekz>
        strikte <tld/>aj paroladoj
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a pasporto
        <fnt>
          <bib>CKv</bib>
        </fnt>;
      </ekz>
      <ekz>
        statuso <tld/>a
        <fnt>
          <bib>ChM</bib>
        </fnt>;
      </ekz>
      <ekz>
        li ne estas devigata, kiel en la <tld/>aj paroladoj, kateni siajn ideojn en
        rigorajn formulojn
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a ceremoniaro preskribas la titoladon al diversaj altranguloj en
        diversaj situacioj
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        la hispana ministraro e&ccirc; invitis oficiale per la vojo <tld/>a &ccirc;iujn registarojn
        alilandajn
        <fnt>
          <bib>VivZam</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="diplom2.0a">diplomatia</ref>
    </refgrp>
  </snc>
  <snc mrk="diplom1.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kvaza&ubreve; <tld/>o:
      <ekz>
        <tld/>a &gcirc;entileco
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        kun eminenta <tld/>a instinkto la sinjorino ne sendis portreton komunan kun la
        edzo
        <fnt>
          <bib>KKr</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="delika.0a.iu">delikata<sncref/></ref>,
      <ref cel="gxenti.0a">&gcirc;entila</ref>,
      <ref cel="sagac.0a.iu">sagaca<sncref/></ref>,
<ref cel="subtil.0a.sente">subtila<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_p;&c_l;&c_ja;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="de">diplomatisch</trd>
  <trd lng="es">diplom&aacute;tico</trd>
  <trd lng="fr">diplomatique<klr> (de diplomate)</klr></trd>
  <trd lng="he">&dalet;&#x5B4;&yod;&pe;&#x5BC;&lamed;&vav;&#x5B9;&mem;&#x5B8;&tet;&yod;</trd>
  <trdgrp lng="ja">
    <trd>&#x5916;&#x4EA4;&#x5B98;&#x306E;  <pr>&#x304C;&#x3044;&#x3053;&#x3046;&#x304B;&#x3093;&#x306E;</pr></trd>,
    <trd>&#x5916;&#x4EA4;&#x7684;&#x306A;  <pr>&#x304C;&#x3044;&#x3053;&#x3046;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x99C6;&#x3051;&#x5F15;&#x304D;&#x306E;&#x3046;&#x307E;&#x3044;  <pr>&#x304B;&#x3051;&#x3072;&#x304D;&#x306E;&#x3046;&#x307E;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="pl">dyplomatyczny</trd>
  <trd lng="pt">diplom&aacute;tico</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_i;&c_p;&c_l;&c_o;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_d;&c_i;&c_p;&c_l;&c_o;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="uk">&#x434;&#x438;&#x43F;&#x43B;&#x43E;&#x43C;&#x430;&#x442;&#x438;&#x447;&#x43D;&#x438;&#x439;</trd>
  <trdgrp lng="zh">
    <trd>&#x5916;&#x4EA4;&#x4E0A; <pr>w&#xE0;iji&#x101;osh&#xE0;ng</pr></trd>,
    <trd>&#x5916;&#x4EA4;&#x65B9;&#x5F0F; <pr>w&#xE0;iji&#x101;of&#x101;ngsh&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>
