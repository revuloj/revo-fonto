<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>&scirc;ekel</rad>/o <fnt><bib>NePiv</bib></fnt>
    </kap>

    <drv mrk="sxekel.0o">
      <kap><tld/>o</kap>
      <snc mrk="sxekel.0o.EKON">
        <uzo tip="fak">EKON</uzo>
        <dif>
          <ref tip="super" cel="unu.mon0o.EKON">Monunuo</ref> de
          <ref cel="israel.I0o.GEOG">Israelo</ref>:
          <ekz>
            Matrix <klr>...</klr> havas 2.300 dungitoj kaj pezas 500 
            milionojn da <tld/>oj (89 milionojn da e&ubreve;roj)  
            <fnt>
              <aut>G. Algazi, trad. V. Lutermano</aut>,
              <vrk>
<url ref="http://eo.mondediplo.com/article1141.html">
Religia ekspluatado</url></vrk>,
              <lok>le Monde diplomatique, 2006-08</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="bg">&c_sh;&c_je;&c_k;&c_je;&c_l;</trd>
      <trd lng="de">Schekel</trd>
      <trd lng="fr">shekel</trd>
      <trdgrp lng="it">
        <trd>sheqel</trd>, 
        <trd>nuovo <ind>siclo</ind> israeliano</trd>, 
        <trd>shekel</trd>
      </trdgrp>
      <trd lng="ru">&c_sh;&c_je;&c_k;&c_je;&c_l;&c_mol;</trd>
    </drv>
  </art>

