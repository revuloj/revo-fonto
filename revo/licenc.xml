<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: licenc.xml,v 1.23 2025/02/28 06:35:39 revo Exp $">
<kap>
  <rad>licenci</rad>/o
  <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="licenc.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PED</uzo>
  <snc>
    <dif>
      <klr>(en kelkaj landoj, fakoj)</klr> Universitata <ref tip="super" cel="grad.0o.sxtupo">grado</ref>,
      meza inter matureca ekzameno kaj magistreco a&ubreve; doktoreco kaj permesanta instruadon:
      <ekz>
        <tld/>ato
        <fnt><bib>JED</bib></fnt>;
      </ekz>
      <ekz>
        li jam en sia 19a jaro estis <tld/>ato pri literaturo, 20-jara li ricevis
        diplomon pri historio de la religioj kaj 22-jara li i&gcirc;is agregaciulo pri
        filologio &ndash; la plej alta grado en la franca instrusistemo
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#8470;6</lok>
        </fnt>;
      </ekz>
      <ekz>
        ke li prezentis identigilajn paperojn en la parlamento kvaza&ubreve; in&gcirc;eniero, kiam li
        ankora&ubreve; ne havis la <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Jo&atilde;o Jos&eacute; Santos</aut>
          <vrk><url ref="&Monato;/2007/009262.php"
              >Diantoj ... kaj dornoj</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        nova membro, <tld/>ito de angla kaj germana, malfermis publikan kaj lernejan
        E-kursojn
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 1 (63)</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="bakala.0o">bakala&ubreve;ro</ref>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_c;&c_e;&c_n;&c_c;&c_y;&c_ja;&c_t; <klr>(&c_n;&c_a;&c_v;&c_u;&c_k;&c_o;&c_v;&c_a;&c_ja;
  &c_s;&c_t;&c_u;&c_p;&c_je;&c_n;&c_mol;)</klr></trd>
  <trd lng="br">aotreegezh</trd>
  <trd lng="ca">llicenciatura</trd>
  <trd lng="cs">licenci&aacute;t</trd>
  <trd lng="de">Lizentiat<klr>
    [akadem. Titel in einigen L&auml;ndern, nicht in DE]</klr></trd>
  <trd lng="en">licence<klr tip="amb">
     (academic title in some countries)</klr></trd>
  <trd lng="es">licenciatura</trd>
  <trdgrp lng="fr">
    <trd>licence<klr> (titre univ.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>licenci&aacute;tus</trd>,
    <trd>fels&odblac;fok&uacute; <ind>v&eacute;gzetts&eacute;g</ind></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#23398;&#22763;&#21495;  <pr>&#12364;&#12367;&#12375;&#12372;&#12358;</pr></trd>,
    <trd>&#23398;&#20301;  <pr>&#12364;&#12367;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">licentiaat<klr tip="ind"> (graad)</klr></trd>
  <trd lng="pl">licencjat</trd>
  <trd lng="pt">licenciatura <klr>(grau universit&aacute;rio em alguns
  pa&iacute;ses)</klr></trd>
  <trd lng="ro">studiile universitare de licen&tcomma;&abreve;</trd>
  <trd lng="ru">&c_l;&c_i;&c_c;&c_je;&c_n;&c_c;&c_i;&c_a;&c_t; (&c_u;&c_ch;&c_jo;&c_n;&c_a;&c_ja; &c_s;&c_t;&c_je;&c_p;&c_je;&c_n;&c_mol;)</trd>
  <trd lng="sk">licenci&aacute;t</trd>
  <trd lng="uk">&c_l;&c_ib;&c_c;&c_je;&c_n;&c_c;&c_ib;&c_a;&c_t; (&c_u;&c_ch;&c_je;&c_n;&c_i;&c_j; &c_s;&c_t;&c_u;&c_p;&c_ib;&c_n;&c_mol;)</trd>
</drv>

<drv mrk="licenc.0ulo">
  <kap><tld/>ulo</kap>
  <uzo tip="fak">PED</uzo>
  <snc>
    <dif>
      Havanto de <tld/>a rango:
      <ekz>
        <tld/>ulino pri sociaj komunikoj kaj profesia instruistino
        <fnt><aut>NikSt</aut>: <vrk>Legu, ludu, ridu! (Gilena, Ludi por
        ridi)</vrk>, <bib>Monato</bib><lok>jaro 1993a, numero 9a, p.
        23a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_c;&c_e;&c_n;&c_c;&c_y;&c_ja;&c_t; <klr>(&c_t;&c_o;&c_j;, &c_h;&c_t;&c_o; &c_a;&c_t;&c_r;&c_y;&c_m;&c_a;&c_w;
  &c_s;&c_t;&c_u;&c_p;&c_je;&c_n;&c_mol;)</klr></trd>
  <trd lng="br">aotreeg</trd>
  <trd lng="ca">llicenciat</trd>
  <trd lng="cs">bakal&aacute;&rcaron;</trd>
  <trd lng="en">licentiate</trd>
  <trd lng="es">licenciado</trd>
  <trdgrp lng="fr">
    <trd>licenci&eacute;<klr> (titre univ.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>licenci&aacute;tusi fokozatot szerzett</trd>,
    <trd>fels&odblac;fok&uacute; <ind>v&eacute;gzetts&eacute;g&udblac;</ind></trd>
  </trdgrp>
  <trd lng="ja">&#23398;&#22763;  <pr>&#12364;&#12367;&#12375;</pr></trd>
  <trd lng="nl">licentiaat<klr tip="ind"> (persoon)</klr></trd>
  <trd lng="pl">baka&lstroke;arz</trd>

  <trd lng="pt">licenciado</trd>
  <trd lng="ro">&icirc;nv&abreve;&tcomma;&abreve;tor</trd>
  <trd lng="ru">&c_l;&c_i;&c_c;&c_je;&c_n;&c_c;&c_i;&c_a;&c_t; (&c_l;&c_i;&c_c;&c_o;,
&c_p;&c_o;&c_l;&c_u;&c_ch;&c_i;&c_v;&c_sh;&c_je;&c_je;
  &c_s;&c_t;&c_je;&c_p;&c_je;&c_n;&c_mol;)</trd>
  <trd lng="sk">bakal&aacute;r</trd>
  <trd lng="uk">&c_l;&c_ib;&c_c;&c_je;&c_n;&c_c;&c_ib;&c_a;&c_t;</trd>
</drv>
</art>
