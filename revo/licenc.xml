<vortaro>
<art mrk="$Id$">
<kap>
  <rad>licenci</rad>/o
</kap>
<drv mrk="licenc.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PED</uzo>
  <snc>
    <dif>
      Universitata rango, meza inter matureca ekzameno kaj doktoreco:
      <ekz>
        <tld/>ato<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_c;&c_e;&c_n;&c_c;&c_y;&c_ja;&c_t; <klr>(&c_n;&c_a;&c_v;&c_u;&c_k;&c_o;&c_v;&c_a;&c_ja; &c_s;&c_t;&c_u;&c_p;&c_je;&c_n;&c_mol;)</klr></trd>
  <trd lng="br">aotreegezh</trd>
  <trd lng="de">Lizentiat<klr> [akadem. Titel in einigen L&auml;ndern, nicht in
  DE]</klr></trd>
  <trdgrp lng="fr">
    <trd>licence<klr> (titre univ.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>licenci&aacute;tus</trd>,
    <trd>fels&odblac;fok&uacute; <ind>v&eacute;gzetts&eacute;g</ind></trd>
  </trdgrp>
  <trd lng="nl">licentiaat</trd>
  <trd lng="pt">licenciatura <klr>(grau universit&aacute;rio em alguns pa&iacute;ses)</klr></trd>
  <trd lng="ru">&c_l;&c_i;&c_c;&c_je;&c_n;&c_c;&c_i;&c_a;&c_t; (&c_u;&c_ch;&c_jo;&c_n;&c_a;&c_ja;
&c_s;&c_t;&c_je;&c_p;&c_je;&c_n;&c_mol;)</trd>
</drv>

<drv mrk="licenc.0ulo">
  <kap><tld/>ulo</kap>
  <uzo tip="fak">PED</uzo>
  <snc>
    <dif>
      Havanto de <tld/>a rango:
      <ekz>
        <tld/>ulino pri sociaj komunikoj kaj profesia instruistino
        <fnt><aut>NikSt</aut>: <vrk>Legu, ludu, ridu! (Gilena, Ludi por
        ridi)</vrk>, <bib>Monato</bib><lok>jaro 1993a, numero 9a, p.
        23a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_c;&c_e;&c_n;&c_c;&c_y;&c_ja;&c_t; <klr>(&c_t;&c_o;&c_j;, &c_h;&c_t;&c_o; &c_a;&c_t;&c_r;&c_y;&c_m;&c_a;&c_w; &c_s;&c_t;&c_u;&c_p;&c_je;&c_n;&c_mol;)</klr></trd>
  <trd lng="br">aotreeg</trd>
  <trdgrp lng="fr">
    <trd>licenci&eacute;<klr> (titre univ.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>licenci&aacute;tusi fokozatot szerzett</trd>,
    <trd>fels&odblac;fok&uacute; <ind>v&eacute;gzetts&eacute;g&udblac;</ind></trd>
  </trdgrp>  
  <trd lng="ru">&c_l;&c_i;&c_c;&c_je;&c_n;&c_c;&c_i;&c_a;&c_t; (&c_l;&c_i;&c_c;&c_o;,
&c_p;&c_o;&c_l;&c_u;&c_ch;&c_i;&c_v;&c_sh;&c_je;&c_je;
  &c_s;&c_t;&c_je;&c_p;&c_je;&c_n;&c_mol;)</trd>
</drv>
</art>
