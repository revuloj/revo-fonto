    <kap>
      <ofc>10</ofc><rad>Galici</rad>/o
    </kap>

<drv mrk="galici1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Historia <ref tip="lst" lst="voko:regionoj" cel="region.0o.land">regiono</ref>
      norde de Karpatoj en la sudo de Polando kaj okcidento de Ukrainujo:
      <ekz>
        en <tld/>o, juda lingvo estis le&gcirc;e nemontrebla kiel la gepatra en deklaroj
        oficialaj
        <fnt>
          <bib>VivZam</bib>
          <lok>Vivo de Zamenhof</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ndash; &Ccirc;u vi partoprenis la batalojn? &ndash; En <tld/>o <klr>[&#8230;]</klr> apud <nom>Lvov</nom>
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Dunajec</nom>, rivero, dekstra alfluo al Vistulo en <tld/>o
        <fnt>
          <bib>SrT</bib>
          <lok>Libro XII.</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Galizien <klr>(Osteuropa)</klr></trd>
    <trd lng="en">Galicia <klr>(Eastern Europe)</klr></trd>
    <trd lng="fr">Galicie</trd>
    <trd lng="pl">Galicja</trd>
    <trd lng="ru">&c_G;&c_a;&c_l;&c_i;&c_c;&c_i;&c_ja;</trd>
    <trd lng="uk">&c_G;&c_a;&c_l;&c_i;&c_ch;&c_i;&c_n;&c_a;</trd>
    <trd lng="yi">&gimel;&alef;&#1463;&lamed;&yod;&tsadi;&yod;&ayin;</trd>
  </snc>
<trdgrp lng="be">
  <trd>&c_G;&c_a;&c_l;&c_ib;&c_c;&c_y;&c_ja;</trd>,
  <trd>&c_G;&c_a;&c_l;&c_ib;&c_ch;&c_y;&c_n;&c_a;</trd>
</trdgrp>
</drv>

