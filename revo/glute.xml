<art mrk="$Id: glute.xml,v 1.24 2023/11/11 11:36:35 revo Exp $">
<kap><rad>glute</rad>/o</kap>
<drv mrk="glute.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="vang.sid0o">Sidvango</ref>:
      <ekz>
        dorso estas parto de la homa korpo, de la &scirc;ultroj &gcirc;is la <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=49071">Dorso</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        bikino estas banvesto por virinoj <klr>[&#x2026;]</klr> el du apartaj pecoj: mamzono
        por kovri la mamojn, kaj kalsono por kovri la pubon kaj foje la <tld/>ojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=50325">Bikino</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_je;&c_d;&c_a;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_ja;&c_g;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">natja</trd>
  <trd lng="cs">h&yacute;&zcaron;d&ecaron;</trd>
  <trdgrp lng="de">
    <trd>Ges&auml;&szlig;backe</trd>,
    <trd>Hinterbacke</trd>,
    <trd>Pobacke</trd>
  </trdgrp>
  <trd lng="es">gl&uacute;teo</trd>
  <trd lng="fr">fesse</trd>
  <trd lng="hu">far</trd>
  <trd lng="it">gluteo</trd>
  <trd lng="ja">&#x81C0;&#x90E8;  <pr>&#x3067;&#x3093;&#x3076;</pr></trd>
  <trd lng="nl">bil</trd>
  <trdgrp lng="pl">
    <trd>pupa</trd>,
    <trd>ty&lstroke;ek</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>n&aacute;degas</trd>,
    <trd>gl&uacute;teo</trd>
  </trdgrp>
  <trd lng="ru">&c_ja;&c_g;&c_o;&c_d;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">zadok <klr>(polovica)</klr></trd>
  <trd lng="uk">&#x441;&#x456;&#x434;&#x43D;&#x438;&#x446;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x9830; <pr>ji&#xE1;</pr></trd>,
    <trd>&#x9824; <pr>y&#xED;</pr></trd>
  </trdgrp>
</drv>
</art>
