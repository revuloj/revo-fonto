<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kalpak</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kalpak.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Origine tatara, pelta <ref tip="super" cel="cxap.0o">&ccirc;apo</ref>,
      supre pli lar&gcirc;a ol malsupre,
      uzita interalie en la 18a kaj 19a jarcentoj kiel armea uniformero:
      <ekz>
        sur &scirc;ia kapo petolis skarlata <tld/>o
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 49a, p. 121a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>colback</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>kalpag <klr tip="ind">(pr&eacute;ms&uuml;veg)</klr></trd>,
     <trd>pr&eacute;ms&uuml;veg</trd>
  </trdgrp>
  <trd lng="pt">colbaque</trd>
</drv>
</art>

