<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kalpak.xml,v 1.10 2023/10/23 09:25:55 revo Exp $">
<kap>
  <rad>kalpak</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="kalpak.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Origine tatara, pelta <ref tip="super" cel="cxap.0o">&ccirc;apo</ref>,
      supre pli lar&gcirc;a ol malsupre,
      uzita interalie en la 18a kaj 19a jarcentoj kiel armea uniformero:
      <ekz>
        sur &scirc;ia kapo petolis skarlata <tld/>o
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 49a, p. 121a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_w;&c_p;&c_a;&c_k;</trd>
  <trd lng="cs">kalpak</trd>
  <trd lng="de">Kalpak</trd>
  <trdgrp lng="fr">
    <trd>colback</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>kalpag <klr tip="ind">(pr&eacute;ms&uuml;veg)</klr></trd>,
     <trd>pr&eacute;ms&uuml;veg</trd>
  </trdgrp>
  <trd lng="pl">ko&lstroke;pak</trd>
  <trd lng="pt">colbaque</trd>
  <trd lng="ro">&#537;apc&abreve;</trd>
  <trd lng="sk">kalpak</trd>
  <trdgrp lng="uk">
    <trd>&c_h;&c_u;&c_t;&c_r;&#x44F;&#x43D;&c_a; &c_sh;&c_a;&#x43F;&#x43A;&c_a;</trd>,
    <trd>&#x43A;&#x43E;&c_v;&#x43F;&c_a;&#x43A;</trd>
  </trdgrp>
</drv>
</art>

