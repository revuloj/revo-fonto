<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: klinik.xml,v 1.18 2019/06/30 18:21:02 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>klinik</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="klinik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc mrk="klinik.0o.instrua">
    <dif>
      Sekcio de <ref tip="malprt" cel="hospit.0o">hospitalo</ref>,
      kie majstro instruas siajn dis&ccirc;iplojn,
      farante anta&ubreve; ili diagnozojn &ccirc;e la litoj de malsanuloj.  
    </dif>
  </snc>
  <snc mrk="klinik.0o.privata">
    <dif>
      Malsanulejo ne&scirc;tata, speciali&gcirc;inta:
      <ekz>
        dum preska&ubreve; unu jaro funkcias en Litovio krimula grupo
        kunlaboranta kun
        nelegalaj <tld/>oj en Italio, al kiuj &gcirc;i liveras homajn organojn
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Straznickas</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007006.html"
              >Litovio: Krimo organ-izata</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en iuj landoj, &ccirc;efe Usono, estas <tld/>oj, kie oni kuracas
        diversajn malsanojn
        (ne nur) psikologiajn per helpo de son&gcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Reza Kheir-khah</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007087.html"
              >Son&gcirc;o: Mirinda dimensio de nia vivo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        aparte furoras &ccirc;e privataj <tld/>oj dentaj kaj
        plasti<klr>[k]</klr>aj operacioj
        <fnt>
          <bib>Monato</bib>
          <aut>Last</aut>
          <vrk><url ref="&Monato;/2006/009000.php"
              >Medicina turismo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la nova <tld/>o estas 100 % private financata
        <fnt>
          <bib>Monato</bib>
          <aut>Betty Chatterjee</aut>
          <vrk><url ref="&Monato;/2011/010482.php"
              >Senrajtulaj sanrajtoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en Ukrainio kostas abortigo en &scirc;tata hospitalo a&ubreve; privata
        <tld/>o ekvivalenton
        de proksimume 100 usonaj dolaroj
        <fnt>
          <bib>Monato</bib>
          <aut>Dmitrij Cibulevskij</aut>
          <vrk><url ref="&Monato;/2007/009089.php"
              >Lviv kaj mort'</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_ib;&c_n;&c_ib;&c_k;&c_a;</trd>
  <trd lng="ca">cl&iacute;nica</trd>
  <trd lng="cs">klinika</trd>
  <trdgrp lng="de">
   <trd>Klinik</trd>,
   <trd>Lehrkrankenhaus</trd></trdgrp>
  <trd lng="en">clinic</trd>
  <trd lng="es">cl&iacute;nica</trd>
  <trd lng="fr">clinique</trd>
  <trd lng="it">clinica<klr> (osped. univ.)</klr></trd>
  <trd lng="nl">kliniek</trd>
  <trd lng="pl">klinika</trd>     
  <trd lng="ro">clinic&abreve;</trd>
  <trd lng="ru">&c_k;&c_l;&c_i;&c_n;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">klinika</trd>
</drv>
</art>
