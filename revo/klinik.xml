<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: klinik.xml,v 1.25 2023/12/03 09:58:50 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>klinik</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="klinik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc mrk="klinik.0o.instrua">
    <dif>
      Sekcio de <ref tip="malprt" cel="hospit.0o">hospitalo</ref>,
      kie majstro instruas siajn dis&ccirc;iplojn,
      farante anta&ubreve; ili diagnozojn &ccirc;e la litoj de malsanuloj.  
    </dif>
  </snc>
  <snc mrk="klinik.0o.privata">
    <dif>
      Malsanulejo ne&scirc;tata, speciali&gcirc;inta:
      <ekz>
        dum preska&ubreve; unu jaro funkcias en Litovio krimula grupo
        kunlaboranta kun
        nelegalaj <tld/>oj en Italio, al kiuj &gcirc;i liveras homajn organojn
        <fnt>
          <aut>last</aut>
            <vrk><url ref="&Monato;/2000/007006.php">Krimo organ-izata</url></vrk>
            <bib>Monato</bib>
            <lok>2000/11, p. 14</lok>
          </fnt>;
      </ekz>
      <ekz>
        en iuj landoj, &ccirc;efe Usono, estas <tld/>oj, kie oni kuracas
        diversajn malsanojn
        (ne nur) psikologiajn per helpo de son&gcirc;oj
        <fnt>
          <aut>Reza Kheir-Khah</aut>
            <vrk><url ref="&Monato;/2000/007087.php">Mirinda dimensio de nia vivo</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 16</lok>
          </fnt>;
      </ekz>
      <ekz>
        aparte furoras &ccirc;e privataj <tld/>oj dentaj kaj
        plasti<klr>[k]</klr>aj operacioj
        <fnt>
          <bib>Monato</bib>
          <aut>Last</aut>
          <vrk><url ref="&Monato;/2006/009000.php">Medicina turismo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la nova <tld/>o estas 100 % private financata
        <fnt>
          <bib>Monato</bib>
          <aut>Betty Chatterjee</aut>
          <vrk><url ref="&Monato;/2011/010482.php">Senrajtulaj sanrajtoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en Ukrainio kostas abortigo en &scirc;tata hospitalo a&ubreve; privata
        <tld/>o ekvivalenton
        de proksimume 100 usonaj dolaroj
        <fnt>
          <bib>Monato</bib>
          <aut>Dmitrij Cibulevskij</aut>
          <vrk><url ref="&Monato;/2007/009089.php">Lviv kaj mort'</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_ib;&c_n;&c_ib;&c_k;&c_a;</trd>
  <trd lng="ca">cl&iacute;nica</trd>
  <trd lng="cs">klinika</trd>
  <trdgrp lng="de">
   <trd>Klinik</trd>,
   <trd>Lehrkrankenhaus</trd></trdgrp>
  <trd lng="en">clinic</trd>
  <trd lng="es">cl&iacute;nica</trd>
  <trd lng="fr">clinique</trd>
  <trd lng="it">clinica<klr> (osped. univ.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x81E8;&#x5E8A;  <pr>&#x308A;&#x3093;&#x3057;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x81E8;&#x5E8A;&#x8B1B;&#x7FA9;  <pr>&#x308A;&#x3093;&#x3057;&#x3087;&#x3046;&#x3053;&#x3046;&#x304E;</pr></trd>,
    <trd>&#x81E8;&#x5E8A;&#x8B1B;&#x7FA9;&#x5BA4;  <pr>&#x308A;&#x3093;&#x3057;&#x3087;&#x3046;&#x3053;&#x3046;&#x304E;&#x3057;&#x3064;</pr></trd>,
    <trd>&#x8A3A;&#x7642;&#x6240;  <pr>&#x3057;&#x3093;&#x308A;&#x3087;&#x3046;&#x3057;&#x3087;</pr></trd>,
    <trd>&#x533B;&#x9662;  <pr>&#x3044;&#x3044;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">kliniek</trd>
  <trd lng="pl">klinika</trd>     
  <trd lng="ro">clinic&abreve;</trd>
  <trd lng="ru">&c_k;&c_l;&c_i;&c_n;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">klinika</trd>
  <trd lng="uk">&#x43A;&#x43B;&c_ib;&#x43D;&c_ib;&#x43A;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x533B;&#x52A1;&#x6240; <pr>y&#x12B;w&#xF9;su&#x1D2;</pr></trd>,
    <trd>&#x533B;&#x52A1;&#x9662; <pr>y&#x12B;w&#xF9;yu&#xE0;n</pr></trd>,
    <trd>&#x9580;&#x8A3A;&#x5BA4; <pr>m&#xE9;nzh&#x11B;nsh&#xEC;</pr></trd>,
    <trd>&#x6559;&#x5B66;&#x533B;&#x9662; <pr>ji&#xE0;oxu&#xE9;y&#x12B;yu&#xE0;n</pr></trd>,
    <trd>&#x533B;&#x52A1;&#x5BA4; <pr>y&#x12B;w&#xF9;sh&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>
