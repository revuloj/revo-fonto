<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: timon.xml,v 1.19 2023/10/21 13:52:03 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>timon</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="timon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="timon.0o.KOMUNE">
    <dif>
      Longa ligna <ref tip="super" cel="stang.0o">stango</ref> fiksita horizontale &ccirc;e
      la anta&ubreve;o de <ref tip="malprt" cel="vetur.0ilo">veturilo</ref> kaj al kiu oni jungas la
      &ccirc;evalojn:
      <ekz>
        li prenis la <tld/>ojn de la veturilo, kaj &scirc;ajne indiferente kaj nesuspekteme
        forruladis &gcirc;in
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        Johan' la Brava la <tld/>on prenis, /
        &Ccirc;aron kvaza&ubreve; lude el la koto trenis
        <fnt>
          <aut>S. Pet&odblac;fi, trad. K. Kalocsay</aut>,
          <vrk><url ref="http://www.egalite.hu/kalocsay/petofi/johano%20la%20brava.htm">
          Johano la brava</url></vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_y;&c_sh;&c_a;&c_l;&c_mol;</trd>, 
    <trd>&c_a;&c_g;&c_l;&c_o;&c_b;&c_l;&c_ja;</trd>
  </trdgrp>
  <trd lng="cs">oj <klr>(vozu)</klr></trd>
  <trd lng="de">Deichsel</trd>
  <trd lng="en">drawbar</trd>
  <trd lng="fr">timon</trd>
  <trdgrp lng="hu">
    <trd>kocsir&uacute;d</trd>,
    <trd>szek&eacute;rr&uacute;d</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x304B;&#x3058;&#x68D2;  <pr>&#x304B;&#x3058;&#x307C;&#x3046;</pr></trd>,
    <trd>&#x306A;&#x304C;&#x3048;</trd>
  </trdgrp>
  <trd lng="nl">dissel</trd>
  <trd lng="pl">dyszel</trd>
  <trdgrp lng="pt">
    <trd>tim&atilde;o<klr> (de carro)</klr></trd>,
    <trd>lan&ccedil;a</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_y;&c_sh;&c_l;&c_o;</trd>,
    <trd>&c_o;&c_g;&c_l;&c_o;&c_b;&c_l;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">oje</trd>
  <trdgrp lng="uk">
    <trd>&#x434;&#x438;&#x448;&#x435;&#x43B;&#x44C;</trd>,
    <trd>&#x433;&#x43E;&#x43B;&#x43E;&#x431;&#x43B;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
