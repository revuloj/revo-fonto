<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>Ko&scirc;i</rad>/o
</kap>
<drv mrk="kosxi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <dif>
      Franclingve: Augustin <trd lng="fr">Cauchy</trd>, 1789-1857. Franca 
      matematikisto.
    </dif>
    <trd lng="be">&c_K;&c_a;&c_sh;&c_y;</trd>
    <trd lng="de">Cauchy</trd>
    <trd lng="en">Cauchy</trd>
    <trd lng="hu">Cauchy</trd>    
    <trd lng="pl">Cauchy</trd>
    <trd lng="pt">Cauchy</trd>
    <trd lng="ru">&c_K;&c_o;&c_sh;&c_i;</trd>
  </snc>
</drv>
<drv mrk="kosxi.k0a">
  <kap><tld lit="k"/>a</kap>
  <snc>
    <refgrp tip="vid">
      <ref cel="vic.kosxia0o"><tld lit="k"/>a vico</ref>.
    </refgrp>
    <trd lng="be">&c_K;&c_a;&c_sh;&c_y;</trd>
    <trd lng="de">Cauchysche</trd>
    <trd lng="en">Cauchy['s]</trd>
    <trd lng="fr">de <ind>Cauchy</ind></trd>
    <trd lng="hu">Cauchy-sor</trd>    
    <trd lng="pl">Cauchy'ego</trd>
    <trd lng="ru">&c_K;&c_o;&c_sh;&c_i;<klr tip="ind"> (&c_r;&c_o;&c_d;. &c_p;&c_a;&c_d;.)</klr></trd>
  </snc>
</drv>
</art>
