<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: kosxi.xml,v 1.16 2023/12/03 09:58:59 revo Exp $">
    <kap>
      <rad>Ko&scirc;i</rad>/o
    </kap>

    <drv mrk="kosxi.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">PERS</uzo>
      <fnt><bib>PIV2</bib></fnt>
      <snc>
        <dif>
          Granda franca matematikisto
          (nacilingve: <trd lng="fr">Augustin Louis
          <ind>Cauchy</ind></trd>, 1789--1857).
        </dif>
        <trd lng="be">&c_K;&c_a;&c_sh;&c_y;</trd>
        <trd lng="bg">&c_K;&c_o;&c_sh;&c_i;</trd>
        <trd lng="de">Cauchy</trd>
        <trd lng="en">Cauchy</trd>
        <trd lng="hu">Cauchy</trd> 
        <trd lng="it">Cauchy</trd>    
        <trd lng="lv">Ogist&#x113;ns <ind>Ko&scaron;&#x12B;</ind></trd>    
        <trd lng="pl">Cauchy</trd>
        <trd lng="pt">Cauchy</trd>
        <trd lng="ru">&c_O;&c_g;&c_ju;&c_s;&c_t;&c_je;&c_n; &c_L;&c_u;&c_i; <ind>&c_K;&c_o;&c_sh;&c_i;</ind></trd>
      </snc>
      <trd lng="zh">&#x67EF;&#x897F; <pr>k&#x113;x&#x12B;</pr></trd>
</drv>

    <drv mrk="kosxi.k0a">
      <kap><tld lit="k"/>a</kap>
      <snc>
        <dif>
          Rilata al <tld/>o, liaj verkoj, teorioj, konceptoj:
          <ekz>
            <ref cel="vic.kosxia0o"><tld lit="k"/>a vico</ref>;
          </ekz><ekz>
            la <tld lit="k"/>a-lorenca distribuo;
          </ekz><ekz>
            la <tld lit="k"/>a teoremo integrala.
          </ekz>
        </dif>
        <trd lng="be">&c_K;&c_a;&c_sh;&c_y;</trd>
        <trd lng="de">Cauchysche</trd>
        <trd lng="en">Cauchy['s]</trd>
        <trd lng="fr">de <ind>Cauchy</ind></trd>
        <trd lng="hu">Cauchy-sor</trd>
        <trd lng="it">di <ind>Cauchy</ind></trd>         
        <trd lng="pl">Cauchy'ego</trd>
        <trd lng="ru">&c_K;&c_o;&c_sh;&c_i;<klr tip="ind"> (&c_r;&c_o;&c_d;. &c_p;&c_a;&c_d;.)</klr></trd>
      </snc>
    </drv>

    <drv mrk="kosxi.k0eco">
      <kap><tld lit="k"/>eco</kap>
      <snc>
        <dif>
          La eco esti <tld lit="k"/>a:
          <ekz>
            oni povas pruvi la <tld lit="k"/>econ de tia vico sen scii
            la valoron de la limeso (se tiu entute ekzistas).
          </ekz>
        </dif>
      </snc>
    </drv>

  </art>
