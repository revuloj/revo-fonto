<art mrk="$Id: decidu.xml,v 1.13 2024/03/22 14:27:37 revo Exp $">
<kap>
<rad>decidu</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="decidu.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="decidu.0a.falfolia">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="dif" cel="foli.fal0a.BOT">Falfolia</ref>:
      <ekz>
        <tld/>aj miksarbaroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=21551">Nigra pino</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        baobabo estas <tld/>a, &gcirc;ia nova foliaro &gcirc;enerale
        aperas je la fino de printempo a&ubreve; je la komenco de la
        somero
        <fnt>
          <aut>d. pardue</aut>,
          <vrk>Re: Baobabo</vrk>,
          <lok><url ref="&SCE;">
          soc.culture.esperanto</url>,
          1996-05-09</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">sommergr&uuml;n <klr>(Baum)</klr></trd>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_s;&c_t;&c_a;&c_p;&c_a;&c_d;&c_n;&c_y;</trd>
  <trdgrp lng="ca">
    <trd>caducifoli</trd>,
    <trd>de fulla caduca</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>listy shazuj&iacute;c&iacute;</trd>,
    <trd>opadav&yacute;</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>caducifolio</trd>,
    <trd>de hoja caduca</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;cidu</trd>
  <trdgrp lng="pl">
    <trd>opadaj&aogonek;cy<klr> (o li&sacute;ciu)</klr></trd>
    <trd>odpadaj&aogonek;cy<klr>
    (o skrzydle owada, o rogu itp.)</klr></trd>
    <trd>wypadaj&aogonek;cy<klr>
    (o skrzydle owada, o rogu, z&eogonek;bie itp.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>caducif&oacute;lio</trd>,
    <trd>caduco <klr>(bot.)</klr></trd>,
    <trd>dec&iacute;duo</trd>,
    <trd>deciduif&oacute;lio</trd>
  </trdgrp>
  <trd lng="sk">opadav&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&#x43F;&c_a;&c_d;&c_a;&#x44E;&c_ch;&c_i;&c_j;</trd>,
    <trd>&#x43E;&#x43F;&c_a;&c_d;&c_a;&#x44E;&c_ch;&c_i;&c_j;</trd>,
    <trd>&c_shch;&#x43E; &#x43E;&c_b;&c_s;&c_i;&#x43F;&c_a;&c_jeu;&c_t;&#x44C;&c_s;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
