<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ab&jcirc;ur</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="abjxur.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="abjxur.0i.KOMUNE">
    <ref tip="dif" cel="konfes.for0i.KOMUNE">forkonfesi</ref>
    <ref tip="dif" cel="agnosk.mal0i.KOMUNE">malagnoski</ref>
    <ekz>
      okaze de bapto la bapto-petanto <tld/>as satanon
      <fnt>
        <aut>B. Eichkorn</aut>,
        <vrk><url
        ref="http://b-eichkorn.homepage.t-online.de/Glossar.htm">
        Kristana Glosaro</url></vrk>,
        <lok>[vidita en 2014]</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="da">
    <trd>afsv&aelig;rge</trd>,
    <trd>abjurere</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>abjurer</trd>,
    <trd>renier</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>odwo&lstroke;a&cacute;</trd>,
    <trd>wyrzec si&eogonek; <ind>pogl&aogonek;d&oacute;w</ind></trd>
  </trdgrp>
  <trd lng="ro">abjura</trd>
</drv>
</art>

