<art mrk="$Id: abjxur.xml,v 1.7 2022/03/11 10:37:22 revo Exp $">
<kap>
  <rad>ab&jcirc;ur</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="abjxur.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="abjxur.0i.KOMUNE">
    <dif>
      <ref tip="dif" cel="konfes.for0i.KOMUNE">Forkonfesi</ref>,
      <ref tip="dif" cel="agnosk.mal0i.KOMUNE">malagnoski</ref>:
      <ekz>
        <tld/>i la paganajn diojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=123668">Toro
          (mitologio)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        okaze de bapto la bapto-petanto <tld/>as satanon
        <fnt>
          <aut>B. Eichkorn</aut>,
          <vrk><url ref="http://b-eichkorn.homepage.t-online.de/Glossar.htm">
          Kristana Glosaro</url></vrk>,
          <lok>[vidita en 2014]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>abschw&ouml;ren</trd>,
      <trd>widerrufen</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="da">
    <trd>afsv&aelig;rge</trd>,
    <trd>abjurere</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>abjurer</trd>,
    <trd>renier</trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5B0;&#x5D4;&#x5B4;&#x5EA;&#x5B0;&#x5DB;&#x5BC;&#x5B7;&#x5D7;&#x5B5;&#x5E9;&#x5C1;</trd>
  <trdgrp lng="pl">
    <trd>odwo&lstroke;a&cacute;</trd>,
    <trd>wyrzec si&eogonek; <ind>pogl&aogonek;d&oacute;w</ind></trd>
  </trdgrp>
  <trd lng="pt">renunciar a</trd>
  <trd lng="ro">abjura</trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x43A;&#x430;&#x441;&#x43E;&#x432;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x456;&#x434;&#x445;&#x438;&#x43B;&#x44F;&#x442;&#x438; &#x44E;&#x440;&#x438;&#x434;&#x438;&#x447;&#x43D;&#x438;&#x43C; &#x440;&#x456;&#x448;&#x435;&#x43D;&#x43D;&#x44F;&#x43C;</trd>
  </trdgrp>
</drv>
