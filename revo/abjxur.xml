<art mrk="$Id: abjxur.xml,v 1.9 2025/03/24 20:40:16 revo Exp $">
<kap>
  <rad>ab&jcirc;ur</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="abjxur.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="abjxur.0i.KOMUNE">
    <dif>
      <ref tip="dif" cel="konfes.for0i.KOMUNE">Forkonfesi</ref>,
      <ref tip="dif" cel="agnosk.mal0i.KOMUNE">malagnoski</ref>:
      <ekz>
        <tld/>i la paganajn diojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=123668">Toro
          (mitologio)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        okaze de bapto la bapto-petanto <tld/>as satanon
        <fnt>
          <aut>B. Eichkorn</aut>,
          <vrk><url ref="http://b-eichkorn.homepage.t-online.de/Glossar.htm">
          Kristana Glosaro</url></vrk>,
          <lok>[vidita en 2014]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>abschw&ouml;ren</trd>,
      <trd>widerrufen</trd>
    </trdgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>&#309;ure malagnoski</ke>, <ke>for&#309;uri</ke>, <ke>forproklami</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_r;&c_a;&c_k;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_a;&c_d;&c_c;&c_u;&c_r;&c_a;&c_c;&c_c;&c_a; <klr>(&c_a;&c_d; &c_s;&c_v;&c_a;&c_ib;&c_h; &c_p;&c_o;&c_g;&c_l;&c_ja;&c_d;&c_a;&c_w;)</klr></trd>
  </trdgrp>
  <trdgrp lng="da">
    <trd>afsv&aelig;rge</trd>,
    <trd>abjurere</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>abjurer</trd>,
    <trd>renier</trd>
  </trdgrp>
  <trd lng="he">&#1500;&#1456;&#1492;&#1460;&#1514;&#1456;&#1499;&#1468;&#1463;&#1495;&#1461;&#1513;&#1473;</trd>
  <trdgrp lng="pl">
    <trd>odwo&lstroke;a&cacute;</trd>,
    <trd>wyrzec si&eogonek; <ind>pogl&aogonek;d&oacute;w</ind></trd>
  </trdgrp>
  <trd lng="pt">renunciar a</trd>
  <trd lng="ro">abjura</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&#1082;&c_a;&c_s;&#1086;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_h;&c_i;&#1083;&#1103;&c_t;&c_i; &#1102;&c_r;&c_i;&c_d;&c_i;&c_ch;&#1085;&c_i;&#1084; &c_r;&c_ib;&c_sh;&c_je;&#1085;&#1085;&#1103;&#1084;</trd>
  </trdgrp>
</drv>
</art>

