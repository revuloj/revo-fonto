<art mrk="$Id$">
<kap><rad>padel</rad>/i</kap>
<drv mrk="padel.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <uzo tip="fak">SPO</uzo>
    <dif>
      Remi per unu sola remilo.
    </dif>
    <trd lng="de">paddeln</trd>
    <trd lng="nl">paddelen</trd>
    <trd lng="ru">&c_g;&c_r;&c_je;&c_s;&c_t;&c_i;</trd>
  </snc>
  <snc>
    <fnt>Z</fnt>
    <dif>
      Pene vadi en &scirc;limo; pla&ubreve;di 2.
    </dif>
    <trd lng="nl">ploeteren</trd>
    <trd lng="ru">&c_sh;&c_l;&c_jo;&c_p;&c_a;&c_t;&c_mol; <klr>(&c_b;&c_r;&c_je;&c_s;&c_t;&c_i; &c_p;&c_o; &c_v;&c_o;&c_d;&c_je;)</klr></trd>
  </snc>
</drv>
</art>
