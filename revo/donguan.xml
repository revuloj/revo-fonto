<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: donguan.xml,v 1.3 2023/02/08 21:27:04 afido Exp $">
    <kap>
      <rad>Donguan</rad>/o
    </kap>

<drv mrk="donguan.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="donguan.0o.urbo">
    <dif>
      Granda <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      en <ref tip="malprt" cel="cxin.CX0ujo">&Ccirc;inujo</ref> &ccirc;e Perlorivero, 
      oriente de Kantono kaj norde de <ref cel="sxenjxen.0o">&Scirc;en&jcirc;eno</ref>:
      <ekz>
        la partiestro solene enmanigis al mi anglalingvan libron pri <tld/>o, &leftquot;urbo de
        feli&ccirc;o&rightquot;
        <fnt>
          <aut>Stanislavo</aut>
          <vrk><url ref="http://www.ipernity.com/blog/stanislavo-belov/854814">ipernity: &Scirc;en&jcirc;eno,
          urbo de komerco</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_D;&c_u;&c_n;&c_g;&c_u;&c_a;&c_n;&c_mol;</trd>
    <trd lng="de">Dongguan</trd>
    <trd lng="en">Dongguan</trd>
    <trd lng="fr">Dongguan</trd>
    <trd lng="zh">&#19996;&#33694;</trd>
  </snc>
</drv>

  </art>

