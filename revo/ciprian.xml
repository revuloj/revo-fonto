<art mrk="$Id: ciprian.xml,v 1.9 2022/07/15 08:38:27 revo Exp $">
<kap>
  <rad>Ciprian</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ciprian.0o">
  <kap><tld/>o</kap>
  <snc mrk="ciprian.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> grekdevena:
      <ekz>
        prave diris <klr>...</klr> sankta <tld/>o: &leftquot;Kutimo sen vero
        estas nur
        malnova eraro&rightquot;
        <fnt>
        <vrk><url ref="http://www.womenpriests.org/ep">Kampanjo por la
        ordinado de virinoj...</url></vrk>,
        <lok>[vidita je 2006-05-08]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_K;&c_ib;&c_p;&c_r;&c_y;&c_ja;&c_n;</trd>,
    <trd>&c_C;&c_y;&c_p;&c_r;&c_y;&c_ja;&c_n;</trd>
  </trdgrp>
  <trd lng="ca">Cebri&agrave;</trd>
  <trd lng="fr">Cyprien</trd>
  <trd lng="pl">Cyprian</trd>
  <trd lng="it">Cipriano</trd>
  <trd lng="pt">Cipriano</trd>
  <trd lng="uk">&#x41A;&c_i;&#x43F;&c_r;&c_ib;&c_a;&#x43D;</trd>
</drv>
</art>
