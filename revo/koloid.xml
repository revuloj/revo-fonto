<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: koloid.xml,v 1.16 2022/03/22 15:44:11 revo Exp $">
<kap>
  <rad>koloid</rad>/o
</kap>

<drv mrk="koloid.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Nomo de likva substanco, kiu tre malfacile trairas organajn
      membranojn:
      <ekz>la gumoj, albumino, gelateno, kolodio.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_jo;&c_ib;&c_d;</trd>
  <trdgrp lng="cs">
    <trd>klihovina</trd>,
    <trd>koloid</trd>
  </trdgrp>
  <trd lng="de">Kolloid</trd>
  <trd lng="en">colloid</trd>
  <trd lng="es">coloide</trd>
  <trd lng="fr">collo&iuml;de</trd>
  <trd lng="nl">collo&iuml;de</trd>
  <trd lng="pl">koloid</trd>     
  <trd lng="pt">col&oacute;ide</trd>
  <trd lng="ro">coloid</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_l;&c_o;&c_i;&c_d;</trd>
  <trd lng="sk">koloid</trd>
  <trd lng="uk">&#x43A;&#x43E;&#x43B;&#x43E;&#x457;&#x434;</trd>
</drv>

<drv mrk="koloid.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Havanta la proprecon de <tld/>o:
      <ekz><tld/>a elektrolito, &hcirc;emio, muelilo.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_jo;&c_ib;&c_d;&c_n;&c_y;</trd>
  <trdgrp lng="en">
    <trd>colloidal</trd>,
    <trd>gluey</trd>
  </trdgrp>
  <trd lng="de">kolloidal</trd>
  <trd lng="es">coloidal</trd>
  <trd lng="fr">collo&iuml;dal</trd>
  <trd lng="nl">collo&iuml;daal</trd>
  <trd lng="pl">koloidowy</trd> 
  <trd lng="ru">&c_k;&c_o;&c_l;&c_l;&c_o;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
