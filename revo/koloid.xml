<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>koloid</rad>/o
</kap>
<drv mrk="koloid.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Nomo de likva substanco, kiu tre malfacile trairas organajn membranojn:
      <ekz>la gumoj, albumino, gelateno, kolodio.</ekz>
    </dif>
  </snc>
  <trd lng="en">colloid</trd>
  <trd lng="nl">collo&iuml;de</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_l;&c_o;&c_i;&c_d;</trd>
</drv>
<drv mrk="koloid.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Havanta la proprecon de <tld/>o:
      <ekz><tld/>a elektrolito, &hcirc;emio, muelilo.</ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>colloidal</trd>,
    <trd>gluey</trd>
  </trdgrp>
  <trd lng="nl">collo&iuml;daal</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_l;&c_o;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
