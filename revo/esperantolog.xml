<art mrk="$Id: esperantolog.xml,v 1.10 2023/10/14 15:49:05 revo Exp $">
<kap>
  <rad>esperantolog</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="esperantolog.0o">
  <kap><tld/>o</kap>
  <snc mrk="esperantolog.0o.ESP">
    <uzo tip="fak">ESP</uzo>
    <dif>
      <ref tip="dif" cel="esperantologi.0isto.ESP">Esperantologiisto</ref>:
      <ekz>
        <tld/>o, vidante la du rivalajn formojn &leftquot;<esc>hospital</esc>&rightquot;, 
        &leftquot;<esc>malsanulej</esc>&rightquot;, devas klarigi la tendencojn, kialojn kaj 
        bezonojn
        <fnt>
          <aut>N. Nicholas</aut>,
          <vrk>Diversa&jcirc;oj</vrk>,
          <lok><url ref="&SCE;">
          soc.culture.esperanto</url>, 1990-12-20</lok></fnt>;
      </ekz>
      <ekz>
        tro multe da informoj, el kiuj parto nur por profundaj <tld/>oj (ekzemple, la
        unuafoja apero de la koncerna vorto)
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2004/008110.html">Longe atendata &ccirc;efverko</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Eugen W&uuml;ster</nom> <klr>[&#x2026;]</klr> estis unu el la plej elstaraj
        interlingvistoj kaj <tld/>oj
        <fnt>
          <aut>D. Blanke</aut>,
          <vrk>Jubileo de Eugen W&uuml;ster</vrk>,
          <lok><url ref="http://esperantic.org/ced/ipi26.htm">
          Informilo por Interlingvistoj, 1998/3, numero 26a</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">esperantolog</trd>
  <trd lng="de">Esperantologe</trd>
  <trd lng="fr">esp&eacute;rantologiste</trd>
  <trd lng="ja">&#x30A8;&#x30B9;&#x30DA;&#x30E9;&#x30F3;&#x30C8;&#x5B66;&#x8005;  <pr>&#x30A8;&#x30B9;&#x30DA;&#x30E9;&#x30F3;&#x30C8;&#x304C;&#x304F;&#x3057;&#x3083;</pr></trd>
  <trd lng="pl">esperantologista</trd>
  <trd lng="pt">esperant&oacute;logo</trd>
  <trd lng="sk">esperantol&oacute;g</trd>
  <trd lng="uk">&#x435;&#x441;&#x43F;&#x435;&#x440;&#x430;&#x43D;&#x442;&#x43E;&#x43B;&#x43E;&#x433;</trd>
</drv>
