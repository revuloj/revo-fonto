<kap>
  <rad>epilobi</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="epilob.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el la familio <ref tip="malprt" cel="enoter1.0oj">enoteracoj</ref>
      (<trd lng="la">Epilobium</trd>):
      <ekz>
        <tld/>o estas genro de du- kaj plur-jaraj herboj kun duope kontra&ubreve;sidantaj
        folioj kaj kvarnombraj epiginaj floroj kaj kun lanohavantaj semoj en kvarangula kapsulo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=20163">Epilobio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="&WCU;/thumb/b/b4/%27Epilobium_hirsutum%27_~_Great_Hairy_Willowherb_at_Shipley%2C_West_Sussex%2C_England_01.JPG/407px-%27Epilobium_hirsutum%27_~_Great_Hairy_Willowherb_at_Shipley%2C_West_Sussex%2C_England_01.JPG" prm="CC BY-SA 4.0"><ind>hirta <tld/>o</ind> (Epilobium hirsutum), Shipley, West Sussex, Anglujo
       <fnt>
         <aut>Acabashi, propra verko</aut>
         <lok><url ref="&WCW;/File:%27Epilobium_hirsutum%27_~_Great_Hairy_Willowherb_at_Shipley,_West_Sussex,_England_01.JPG">&WCW;/File:%27Epilobium_hirsutum%27_~_Great_Hairy_Willowherb_at_Shipley,_West_Sussex,_England_01.JPG</url></lok>
       </fnt>
    <trd lng="de">zottiges Weidenr&ouml;schen</trd>
    <trd lng="hu">borzas f&uuml;zike</trd>
    <trd lng="la">Epilobium hirsutum</trd>
  </bld>
  <trd lng="be">&c_s;&c_k;&c_r;&c_y;&c_p;&c_je;&c_n;&c_mol;</trd>
  <trd lng="de">Weidenr&ouml;schen</trd>
  <trdgrp lng="en">
    <trd>willow-herb</trd>,
    <trd>willowherb</trd>,
    <trd>fireweed</trd>
  </trdgrp>
  <trd lng="fr">&eacute;pilobe</trd>
  <trdgrp lng="hu">
    <trd>f&uuml;zike</trd>,
    <trd>der&eacute;ce</trd>
  </trdgrp>
  <trd lng="ja">&#x30A2;&#x30AB;&#x30D0;&#x30CA;</trd>
  <trd lng="pl">wierzbownica</trd>
  <trd lng="pt">epil&oacute;bio</trd>
  <trd lng="ru">&c_k;&c_i;&c_p;&c_r;&c_je;&c_j;</trd>
  <trd lng="sk">vrbovka</trd>
  <trd lng="cs">vrbovka</trd>
  <trd lng="uk">&#x437;&#x43D;&#x456;&#x442;</trd>
</drv>
