<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>nektar</rad>/o
</kap>
<drv mrk="nektar.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MIT</uzo>
    <dif>
      Trinka&jcirc;o de la olimpaj dioj.
    </dif>
    <refgrp tip="sin">
      <ref cel="ambroz.0o">ambrozio</ref>.
    </refgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tre bongusta trinka&jcirc;o.
    </dif>
  </snc>
  <snc>
    <dif>
      Dol&ccirc;a suko sekreciita de floroj, el kiu la abeloj faras mielon.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_e;&c_k;&c_t;&c_a;&c_r;</trd>
  <trd lng="de">Nektar</trd>
  <trd lng="el">&ny;&epsilon_ton;&kappa;&tau;&alfa;&rho;</trd>
  <trd lng="es">n&eacute;ctar</trd>
  <trd lng="hu">nekt&aacute;r</trd>
  <trd lng="nl">nectar</trd>
  <trd lng="pt">n&eacute;ctar</trd>
  <trd lng="ru">&c_n;&c_je;&c_k;&c_t;&c_a;&c_r;</trd>
</drv>
</art>
