<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>fanfaron</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fanfar1.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="fanfar1.0i.sxajnigi">
    <dif>
      Sin glori pro afektataj meritoj, kiujn oni ne posedas:
      <ekz>
        <tld/>i pri kura&gcirc;o<fnt>Z</fnt>, potenco<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i kura&gcirc;on<fnt>Z</fnt>, potencon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i per citatoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i per dono, kiun oni ne faras
        <fnt><bib>MT</bib>, <lok>&Sen; 25:14</lok></fnt>;
      </ekz>
      <ekz>
        li <tld/>is, ke neniu povos lin venki<fnt>Z</fnt>.
      </ekz> 
    </dif>
    <refgrp tip="vid">
      <ref cel="arog.0i">arogi</ref>,
      <ref cel="preten.0i">pretendi</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="knar.0i">knari</ref>,
      <ref cel="son.0i">soni</ref>.
    </refgrp>
  </snc>
  <snc mrk="fanfar1.0i.trofieri">
    <dif>
      Sin mem bombaste la&ubreve;di:
      <ekz>ne <tld/>u irante, <tld/>u
      revenante<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        <tld/>i per sia granda ri&ccirc;eco
        <fnt><bib>MT</bib>, <lok>&Psa; 49:5</lok></fnt>.
      </ekz> 
    </dif>
    <refgrp tip="vid">
      <ref cel="sxvel.brusto0i.FRAZ">brusto&scirc;veli</ref>,
      <ref cel="fier.0i">fieri</ref>,
      <ref cel="glor.sin0i">sin glori</ref>,
      <ref cel="parad.0i">paradi</ref>,
      <ref cel="pav.0i.FIG">pavi<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_v;&c_a;&c_l;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_h;&c_v;&c_a;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>angeben</trd>,
    <trd>sich <ind>br&uuml;sten</ind></trd>
  </trdgrp>
  <trd lng="en">boast</trd>
  <trd lng="es">fanfarronear</trd>
  <trdgrp lng="fr">
    <trd>fanfaronner</trd>,
    <trd>se <ind>vanter</ind><klr tip="ind"> (plastronner)</klr></trd>,
    <trd>plastronner<klr tip="ind"> (se vanter)</klr></trd></trdgrp>
  <trdgrp lng="hu">
    <trd>dicsekszik</trd>,
    <trd>felv&aacute;g<klr tip="ind"> (dicsekszik)</klr></trd>,
    <trd>nagyzol</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>opscheppen<klr tip="ind"> (pochen)</klr></trd>,
    <trd>pochen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>che&lstroke;pi&cacute; si&eogonek;</trd>,
    <trd>przechwala&cacute; si&eogonek;</trd>,
    <trd>popisywa&cacute; si&eogonek;</trd>,
    <trd>pyszni&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fanfarronar</trd>,
    <trd>fanfarrear</trd>,
    <trd>bazofiar</trd>,
    <trd>gabar-se</trd>,
    <trd>ufanar-se</trd>,
    <trd>vangloriar-se</trd>,
    <trd>alardear-se</trd>,
    <trd>louvar-se</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_h;&c_v;&c_a;&c_s;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_a;&c_h;&c_v;&c_a;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chvastať<klr>(sa)</klr></trd>,
    <trd>chváliť sa</trd>
  </trdgrp>
</drv>

<drv mrk="fanfar1.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="fanfar1.0ado.KOMUNE">
    <dif>
      Sintenado de <tld/>ulo:
      <ekz>
        li tion faris nur pro <tld/>ado.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_h;&c_v;&c_a;&c_l;&c_ja;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_h;&c_v;&c_a;&c_l;&c_mol;&c_b;&c_a;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_a;&c_h;&c_v;&c_a;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="fr">vantardise</trd>
  <trdgrp lng="hu">
    <trd>dicsekv&eacute;s</trd>,
    <trd>felv&aacute;g&aacute;s</trd>,
    <trd>nagyzol&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">ijdelheid</trd>
  <trdgrp lng="pl">
    <trd>fanfaronada</trd>,
    <trd>che&lstroke;pienie si&eogonek;</trd>,
    <trd>przechwa&lstroke;ki</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_h;&c_v;&c_a;&c_s;&c_t;&c_o;&c_v;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_b;&c_a;&c_h;&c_v;&c_a;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">vychvaľovanie sa</trd>
</drv>

<drv mrk="fanfar1.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="fanfar1.0ajxo.KOMUNE">
    <dif>
      Ago de <tld/>ulo:
      <ekz>
        tiu minaco estas nur <tld/>a&jcirc;o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_h;&c_v;&c_a;&c_l;&c_ja;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_h;&c_v;&c_a;&c_l;&c_mol;&c_b;&c_a;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_a;&c_h;&c_v;&c_a;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Angeberei</trd>
  <trd lng="fr">vantardise</trd>
  <trd lng="hu">dicsekv&eacute;s</trd>
  <trd lng="pl">przechwa&lstroke;ka</trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_v;&c_a;&c_s;&c_t;&c_o;&c_v;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_b;&c_a;&c_h;&c_v;&c_a;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="fanfar1.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="fanfar1.0ulo.KOMUNE">
    <dif>
      <tld lit="F"/>anta homo.
    </dif>
    <refgrp tip="vid">
      <ref cel="arogan.0a">aroganta</ref>,
      <ref cel="modest">malmodesta</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_v;&c_a;&c_l;&c_mol;&c_k;&c_o;</trd>,
    <trd>&c_v;&c_y;&c_h;&c_v;&c_a;&c_l;&c_ja;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_a;&c_h;&c_v;&c_a;&c_l;</trd>
  </trdgrp>
  <trd lng="de">Angeber</trd>
  <trd lng="es">fanfarr&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>fanfaron</trd>,
    <trd>vantard</trd></trdgrp>
  <trd lng="hu">dicsekv&odblac;</trd>
  <trdgrp lng="nl">
    <trd>pocher</trd>,
    <trd>opschepper</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>fanfaron</trd>,
    <trd>samochwa&lstroke;</trd>,
    <trd>pysza&lstroke;ek</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_h;&c_v;&c_a;&c_s;&c_t;&c_u;&c_n;</trd>,
    <trd>&c_b;&c_a;&c_h;&c_v;&c_a;&c_l;</trd>,
    <trd>&c_f;&c_a;&c_n;&c_f;&c_a;&c_r;&c_o;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chvastúň</trd>,
    <trd>vetroplach</trd>
  </trdgrp>
</drv>
</art>
