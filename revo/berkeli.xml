<vortaro>
<art mrk="$Id$">
<kap>
  <rad>berkeli</rad>/o
</kap>
<drv mrk="berkeli.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
     &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
     atomnumero 97, atompezo 247,0703, simbolo Bk. <ref tip="super"
     cel="urani.trans0o">Transuranio</ref>. &Ccirc;iuj izotopoj de tiu
elemento
     estas radioaktivaj.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_e;&c_r;&c_k;&c_l;&c_ib;&c_j;</trd>
  <trd lng="de">Berkelium</trd>
  <trd lng="en">berkelium</trd>
  <trd lng="hu">berk&eacute;lium</trd>
  <trd lng="nl">berkelium</trd>
  <trd lng="ru">&c_b;&c_je;&c_r;&c_k;&c_l;&c_i;&c_j;</trd>
  <trd lng="sk">berk&eacute;lium</trd>
</drv>
</art>

