<vortaro>
<art mrk="$Id$">
<kap>
  <rad>berkeli</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="berkeli.0o">
  <kap><tld/>o</kap>
  <mlg>Bk</mlg>
  <snc mrk="berkeli.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref>,
      <ref tip="super" cel="urani.trans0o">transuranio</ref> kun
      atomnumero 97, atompezo 247,0703, simbolo <frm>Bk</frm>.
      &Ccirc;iuj <ref tip="super" cel="izotop.0o">izotopoj</ref> 
      de tiu elemento estas radioaktivaj. Aktinoido.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_e;&c_r;&c_k;&c_l;&c_ib;&c_j;</trd>      
  <trd lng="br">berkeliom</trd>
  <trd lng="de">Berkelium</trd>
  <trd lng="en">berkelium</trd>
  <trd lng="es">berquelio</trd>
  <trd lng="fr">berkelium</trd>
  <trd lng="hu">berk&eacute;lium</trd>
  <trd lng="it">berkelio</trd>
  <trd lng="la">Berkelium</trd>
  <trd lng="nl">berkelium</trd>
  <trd lng="pl">berkel</trd>
  <trd lng="pt">berqu&eacute;lio</trd>
  <trd lng="ru">&c_b;&c_je;&c_r;&c_k;&c_l;&c_i;&c_j;</trd>
  <trd lng="sk">berk&eacute;lium</trd>
</drv>
</art>
