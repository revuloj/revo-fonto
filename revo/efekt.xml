<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: efekt.xml,v 1.63 2021/06/18 20:41:32 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>efekt</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="efekt.0o">
  <kap><tld/>o</kap>
  <snc mrk="efekt.0o.KOMUNE">
    <dif>
      Forta <ref tip="super" cel="impres.0o">impreso</ref> farita sur
      la koron a&ubreve; spiriton, precipe de belarta verko, spektaklo
      kc:
      <ekz>
        tiajn aerajn fenomenojn ili neniam anta&ubreve;e vidis <klr>[&#x2026;]</klr>, kian <tld/>on
        &ccirc;io faris
        <fnt>
          <bib>Fab1</bib>
          <lok>Fluganta kofro</lok>
        </fnt>;
      </ekz>
      <ekz>
        korvoj kaj kornikoj faradis bonan <tld/>on sur la blanka suba&jcirc;o <klr>[de
        ne&gcirc;o]</klr>
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de la jaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili e&ccirc; mem ne esperas doni ion pretan kaj ta&ubreve;gan, sed ili volas nur fari <tld/>on
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tio]</klr> faris impreson de ia <klr>[&#x2026;]</klr> pepado, kiu elirante el
        bu&scirc;o de viroj, faris <tld/>on tute neordinaran
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="drast.0a">drasta</ref>
    </refgrp>
    <rim>
      Ne konfuzu kun <ref cel="efik.0o">efiko</ref>.
    </rim>
  </snc>
  <trd lng="af">uitwerking</trd>
  <trd lng="am">&#x12CD;&#x1324;&#x1275;</trd>
  <trd lng="ar">&#x62A;&#x623;&#x62B;&#x64A;&#x631;</trd>
  <trd lng="az">t&#x259;siri</trd>
  <trdgrp lng="be">
     <trd>&c_e;&c_f;&c_e;&c_k;&c_t;</trd>,
     <trd>&c_u;&c_r;&c_a;&c_zh;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
     <trd>&c_u;&c_z;&c_mol;&c_d;&c_z;&c_je;&c_ja;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_je;&c_f;&c_je;&c_k;&c_t;</trd>,
    <trd>&c_v;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="bn">&#x9AA;&#x9CD;&#x9B0;&#x9AD;&#x9BE;&#x9AC;</trd>
  <trd lng="bs">u&#x10D;inak</trd>
  <trd lng="co">effettu</trd>
  <trdgrp lng="cs">
    <trd>dojem</trd>,
    <trd>efekt</trd>,
    <trd>vliv</trd>,
    <trd>&uacute;&ccaron;inek</trd>
  </trdgrp>
  <trd lng="da">effekt</trd>
  <trdgrp lng="de">
    <trd>Effekt</trd>,
    <trd>Eindruck <klr>(Efffekt)</klr></trd>,
  </trdgrp>
  <trdgrp lng="en">
    <trd>effect</trd>,
    <trd>impression</trd>
  </trdgrp>
  <trd lng="et">m&#xF5;ju</trd>
  <trd lng="eu">efektu</trd>
  <trdgrp lng="fr">
    <trd>effet<klr tip="amb"> (impression frappante)</klr></trd>,
    <trd>effet<klr tip="amb"> (de style)</klr></trd>,
    <trd>impression<klr tip="ind"> (que fait qc)</klr></trd></trdgrp>
  <trd lng="fy">effekt</trd>
  <trd lng="gl">efecto</trd>
  <trd lng="gu">&#xA85;&#xAB8;&#xAB0;</trd>
  <trd lng="ha">sakamako</trd>
  <trd lng="hi">&#x92A;&#x94D;&#x930;&#x92D;&#x93E;&#x935;</trd>
  <trd lng="hr">posljedica</trd>
  <trdgrp lng="hu">
    <trd>hat&aacute;s</trd>,
    <trd>effektus</trd>
  </trdgrp>
  <trd lng="hy">&#x561;&#x566;&#x564;&#x565;&#x581;&#x578;&#x582;&#x569;&#x575;&#x578;&#x582;&#x576;</trd>
  <trd lng="is">&#xE1;hrif</trd>
  <trd lng="ja">&#x52B9;&#x679C;</trd>
  <trd lng="jw">efek</trd>
  <trd lng="ka">&#x10D4;&#x10E4;&#x10D4;&#x10E5;&#x10E2;&#x10D8;</trd>
  <trd lng="km">&#x1795;&#x179B;&#x1794;&#x17C9;&#x17C7;&#x1796;&#x17B6;&#x179B;&#x17CB;</trd>
  <trd lng="ko">&#xD6A8;&#xACFC;</trd>
  <trd lng="lo">&#xE9C;&#xEBB;&#xE99;&#xE81;&#xEB0;&#xE97;&#xEBB;&#xE9A;</trd>
  <trd lng="ml">&#xD2B;&#xD32;&#xD02;</trd>
  <trd lng="mr">&#x92A;&#x930;&#x93F;&#x923;&#x93E;&#x92E;</trd>
  <trd lng="my">&#x1021;&#x1000;&#x103B;&#x102D;&#x102F;&#x1038;</trd>
  <trd lng="ne">&#x92A;&#x94D;&#x930;&#x92D;&#x93E;&#x935;</trd>
  <trdgrp lng="nl">
    <trd>effect</trd>,
    <trd>indruk</trd>
  </trdgrp>
  <trd lng="ny">kwenikweni</trd>
  <trd lng="pa">&#xA2A;&#xA4D;&#xA30;&#xA2D;&#xA3E;&#xA35; &#xA28;&#xA42;&#xA70;</trd>
  <trd lng="pl">efekt</trd>
  <trd lng="ps">&#x627;&#x63A;&#x6D0;&#x632;</trd>
  <trdgrp lng="pt">
    <trd>efeito</trd>,
    <trd>impress&atilde;o <klr tip="amb">(sobre o
    esp&iacute;rito)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_f;&c_f;&c_je;&c_k;&c_t;</trd>,
    <trd>&c_v;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sd">&#x627;&#x62B;&#x631;</trd>
  <trd lng="si">&#xD9A;&#xDCA;&#xDBB;&#xDD2;&#xDBA;&#xDCF;&#xDAD;&#xDCA;&#xDB8;&#xD9A;</trd>
  <trdgrp lng="sk">
    <trd>p&ocirc;sobenie</trd>,
    <trd>&uacute;&ccaron;inok</trd>
  </trdgrp>
  <trd lng="sq">efekti</trd>
  <trd lng="st">e le hantle</trd>
  <trd lng="ta">&#xBB5;&#xBBF;&#xBB3;&#xBC8;&#xBB5;&#xBC1;</trd>
  <trd lng="te">&#xC2A;&#xC4D;&#xC30;&#xC2D;&#xC3E;&#xC35;&#xC02;</trd>
  <trd lng="th">&#xE1C;&#xE25;&#xE01;&#xE23;&#xE30;&#xE17;&#xE1A;</trd>
  <trd lng="tl">epekto</trd>
  <trd lng="ur">&#x627;&#x62B;&#x631;</trd>
  <trd lng="vi">hi&#x1EC7;u l&#x1EF1;c</trd>
  <trd lng="yi">&#x5D5;&#x5D5;&#x5D9;&#x5E8;&#x5E7;&#x5D5;&#x5E0;&#x5D2;</trd>
  <trd lng="zu">umphumela</trd>
</drv>

<drv mrk="efekt.0a">
  <kap><tld/>a</kap>
  <snc mrk="efekt.0a.KOMUNE">
    <dif>
      Ka&ubreve;zanta <tld/>on, nepre vekanta atenton kaj emocion:
      <ekz>
        <tld/>a kaj interesega tuta&jcirc;o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la rakontado estas kurta, <tld/>a
        <fnt>
          <aut>Z. Martinov</aut>,
          <vrk><url ref="http://esperanto.net/literaturo/roman/libr/arnecefidreczm.html">
          El la pagana kredo al la kristanismo</url></vrk>,
          <lok>La Espero, 2005:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiuj <tld/>aj dinosa&ubreve;roj &scirc;ajnas mirinde realaj, vivantaj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9402-filmotondro.txt">Filmotondro</url></vrk>
          <lok>94/02</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj temoj estas pli ofte maturaj ol a&ubreve;guraj, pli rememoraj ol a&ubreve;roraj, pli
        retrospektaj ol art<tld/>aj
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="https://www.monato.be/2004/008066.html">Retrorigardo al intensa vivo</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>grand<tld/>a</ind> frazo, titolo.
        <trdgrp lng="fr">
          <trd>emphatique</trd>,
          <trd>ronflant</trd></trdgrp> 
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="efekti.0a">efektiva</ref>,
      <ref cel="efik.0a">efika</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>effektvoll</trd>,
    <trd>eindrucksvoll</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>qui fait de l'<ind>effet</ind></trd>,
    <trd>spectaculaire</trd>
  </trdgrp>
  <trd lng="he">&#x5DE;&#x5E8;&#x5E9;&#x5D9;&#x5DD;</trd>
</drv>

<drv mrk="efekt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="efekt.0i.KOMUNE">
    <dif>
      Ka&ubreve;zi <tld/>on, iel <ref tip="super" cel="impres.0i">impresi</ref>:
      <ekz>
        la foto kaptas momenton de intimeco kaj dol&ccirc;eco, kaj aparte 
        <tld/>as pro sia ludo per speguloj
        <fnt>
          <aut>-</aut>,
          <vrk>Foto pri geja vivokazo...</vrk>,
          <lok><url ref="http://www.lsgesperanto.org/143/forumo143.pdf">
          Forumo, 2009:2-3 (143)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">beeindrucken</trd>
  <trd lng="fr">faire un <ind>effet</ind></trd>
  <trd lng="he">&#x5DC;&#x5D4;&#x5E8;&#x5E9;&#x5D9;&#x5DD;</trd>
</drv>
</art>
