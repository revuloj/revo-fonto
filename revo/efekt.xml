  </trdgrp>
  <trdgrp lng="en">
    <trd>effect</trd>,
    <trd>impression</trd>
  </trdgrp>
  <trd lng="et">m&otilde;ju</trd>
  <trd lng="eu">efektu</trd>
  <trdgrp lng="fr">
    <trd>effet<klr tip="amb"> (impression frappante)</klr></trd>,
