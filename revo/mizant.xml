<drv mrk="mizant.0a">
  <kap><tld/>a</kap>
  <snc mrk="mizant.0a.MOR">
    <dif>
      Nesociema:
      <ekz>
        ne sciis revi lia animo, elegante seka kaj pasia, <tld/>a la&ubreve; la franca maniero
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i, tiel <tld/>a kaj orgojla, ke neniu povas pla&ccirc;i al &scirc;i
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 63a, p.
        369a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_z;&c_a;&c_n;&c_t;&c_r;&c_a;&c_p;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">misantr&ograve;pic</trd>
  <trdgrp lng="de">
    <trd>menschenfeindlich</trd>,
    <trd>misanthrop</trd>
  </trdgrp>
  <trd lng="en">misanthropic</trd>
  <trdgrp lng="fr">
    <trd>insociable</trd>,
    <trd>misanthrope<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>asociaal</trd>,
    <trd>misantropisch</trd>
  </trdgrp>
  <trd lng="pl">odludny</trd>
  <trd lng="ro">solitar</trd>
  <trdgrp lng="zh">
    <trd>&#x5B64;&#x50FB; <pr>g&umacron;p&igrave;</pr></trd>,
    <trd>&#x538C;&#x6076;&#x4EBA;&#x7C7B;&#x8005; <pr>y&agrave;nw&ugrave;r&eacute;nl&egrave;izh&ecaron;</pr></trd>,
