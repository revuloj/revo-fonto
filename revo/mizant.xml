<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mizant.xml,v 1.19 2020/09/30 18:32:55 revo Exp $">
<kap>
  <rad>mizantrop</rad>/o <fnt>B</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="mizant.0o">
  <kap><tld/>o</kap>
  <snc mrk="mizant.0o.MOR">
    <dif>
      Homevitulo,
      <ref tip="vid" cel="soci.kontraux0ulo.MOD">kontra&ubreve;sociulo</ref>,
      <ref tip="vid" cel="ermit.0o.FIG">ermito<sncref/></ref>:
      <ekz>
        &leftquot;Al <tld lit="M"/>oj&rightquot;
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="http://steloj.de/esperanto/butiko"
              >La Stranga Butiko</url></vrk>
          <lok>Al Mizantropoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_z;&c_a;&c_n;&c_t;&c_r;&c_o;&c_p;</trd>
  <trd lng="ca">misantrop</trd>
  <trd lng="cs">mizantrop</trd>
  <trdgrp lng="de">
    <trd>Menschenhasser</trd>,
    <trd>Menschenfeind</trd>,
    <trd>Misanthrop</trd>
  </trdgrp>
  <trd lng="en">misanthrope</trd>
  <trd lng="fr">misanthrope<klr> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>emberker&uuml;l&odblac;</trd>,
    <trd>mizantr&oacute;p</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>mensenhater</trd>,
    <trd>misantroop</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>mizantrop</trd>,
    <trd>odludek</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>misantropo</trd>,
   <trd>antrop&oacute;fobo</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>mizantrop</trd>,
    <trd>pustnic</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_i;&c_z;&c_a;&c_n;&c_t;&c_r;&c_o;&c_p;</trd>
  <trdgrp lng="sk">
    <trd>mizantrop</trd>,
    <trd>od&lcaron;ud</trd>
  </trdgrp>
</drv>

<drv mrk="mizant.0a">
  <kap><tld/>a</kap>
  <snc mrk="mizant.0a.MOR">
    <dif>
      Nesociema:
      <ekz>
        &scirc;i, tiel <tld/>a kaj orgojla, ke neniu povas pla&ccirc;i al &scirc;i
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 63a, p.
        369a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_z;&c_a;&c_n;&c_t;&c_r;&c_a;&c_p;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">misantr&ograve;pic</trd>
  <trd lng="en">misanthropic</trd>
  <trdgrp lng="fr">
    <trd>insociable</trd>,
    <trd>misanthrope<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>asociaal</trd>,
    <trd>misantropisch</trd>
  </trdgrp>
  <trd lng="pl">odludny</trd>     
  <trd lng="ro">solitar</trd>
</drv>
</art>
