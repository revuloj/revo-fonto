<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>logotip</rad>/o
</kap>
<drv mrk="logotip.0o">
  <kap><tld/>o</kap>
  <snc mrk="logotip.0o.simbolo">
    <dif>
      <ref tip="super" cel="emblem.0o.simbolo">Emblemo</ref> 
      entenanta la nomon de firmao, institucio, komerca 
      marko, evento...:
      <ekz>
        &ccirc;u estu konferenca &scirc;ildo <klr>a&ubreve;</klr> <tld/>o
        <fnt>
          <aut>R. Corsetti</aut>,
          <vrk>Konferenca memorigilo</vrk>,
          <lok><url
          ref="http://groups.google.be/group/ilei-konferenco-2008">
          ilei-konferenco 2008</url>, 2006-05-26</lok>
        </fnt>?
      </ekz>
      <ekz>
        la E&ubreve;ropa Komisiono elektis <tld/>on por celebri la 50an 
        datrevenon de la E&ubreve;ropa Unui&gcirc;o
        <fnt>
          <aut>K. Berga</aut>,
          <vrk><url ref="http://labekbotelo.blogspot.com">
          La Bekbotelo</url></vrk>,
          <lok>2006-11-06</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">logotyp</trd>
  <trdgrp lng="en">
    <trd>logo</trd>,
    <trd>logotype</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_n;&a_A;&a_m;&a_w;&a_A;&a_r;&a_h;</trd>,
    <trd>&a_l;&a_w;&f_g;&a_w;</trd>
  </trdgrp>
  <trd lng="fr">logo</trd>
  <trdgrp lng="nl">
    <trd>beeldmerk</trd>,
    <trd>logo</trd>,
    <trd>logogram</trd>
  </trdgrp>
  <trd lng="sk">logotyp</trd>
</drv>
</art>

