<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: logotip.xml,v 1.11 2024/01/15 19:32:00 revo Exp $">
<kap>
  <rad>logotip</rad>/o
</kap>
<drv mrk="logotip.0o">
  <kap><tld/>o</kap>
  <snc mrk="logotip.0o.simbolo">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Bilda kaj plejofte anka&ubreve; nom-enhava
      <ref tip="super" cel="emblem.0o.simbolo">emblemo</ref>     
      de firmao, institucio, komerca marko, evento...:
      <ekz>
        &ccirc;u estu konferenca &scirc;ildo <klr>a&ubreve;</klr> <tld/>o
        <fnt>
          <aut>R. Corsetti</aut>,
          <vrk>Konferenca memorigilo</vrk>,
          <lok><url ref="http://groups.google.be/group/ilei-konferenco-2008">
          ilei-konferenco 2008</url>, 2006-05-26</lok>
        </fnt>?
      </ekz>
      <ekz>
        la E&ubreve;ropa Komisiono elektis <tld/>on por celebri la 50an 
        datrevenon de la E&ubreve;ropa Unui&gcirc;o
        <fnt>
          <aut>K. Berga</aut>,
          <vrk><url ref="http://labekbotelo.blogspot.com">
          La Bekbotelo</url></vrk>,
          <lok>2006-11-06</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="insign.0o.signo">insigno</ref> 
    <refgrp tip="vid">
      <ref cel="mark.0o.produkto">marko<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_g;&c_a;&c_t;&c_y;&c_p;</trd>
  <trd lng="cs">logotyp</trd>
  <trdgrp lng="de">
    <trd>Logo</trd>,
    <trd>Wort-Bild-Marke</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>logo</trd>,
    <trd>logotype</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_n;&a_A;&a_m;&a_w;&a_A;&a_r;&a_h;</trd>,
    <trd>&a_l;&a_w;&f_g;&a_w;</trd>
  </trdgrp>
  <trd lng="fr">logo</trd>
  <trdgrp lng="nl">
    <trd>beeldmerk</trd>,
    <trd>logo</trd>,
    <trd>logogram</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>logotyp</trd>,
    <trd>emblemat</trd>
  </trdgrp> 
  <trd lng="ro">emblem&abreve;</trd>
  <trd lng="sk">logotyp</trd>
  <trdgrp lng="zh">
    <trd>&#x5FBD; <pr>hu&#x12B;</pr></trd>,
    <trd>&#x6807;&#x5FD7; <pr>bi&#x101;ozh&#xEC;</pr></trd>,
    <trd>&#x6A19;&#x8A8C; <pr>bi&#x101;ozh&#xEC;</pr></trd>,
    <trd>&#x5546;&#x6807; <pr>sh&#x101;ngbi&#x101;o</pr></trd>,
    <trd>&#x5546;&#x6A19; <pr>sh&#x101;ngbi&#x101;o</pr></trd>,
    <trd>&#x5FBD;&#x7AE0; <pr>hu&#x12B;zh&#x101;ng</pr></trd>
  </trdgrp>
</drv>
</art>

