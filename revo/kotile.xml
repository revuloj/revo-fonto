<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>kotiledon</rad>/o
</kap>
<drv mrk="kotile.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Unua folio &ccirc;e la semoplantoj, formita jam en
      la <ref tip="malprt" cel="sem.0grajno">semgrajno</ref>;
      &gcirc;ermfolio:
      <ekz>
        la gimnospermoj havas plurajn <tld/>ojn.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Keimblatt</trd>
  <trd lng="la">cotyledon</trd>
</drv>
<drv mrk="kotile.unu0aj">
  <kap><tld/>a</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Unu el la du klasoj de
      <ref tip="super" cel="angios.0oj">angiospermoj</ref>
      (<trd lng="la">Monocotelydoneae</trd>), havantaj
      nur unu <tld/>on.
    </dif>
  </snc>
  <trd lng="de">Einkeimbl&auml;ttrige</trd>
</drv>
<drv mrk="kotile.du0aj">
  <kap><tld/>a</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Unu el la du klasoj de
      <ref tip="super" cel="angios.0oj">angiospermoj</ref>
      (<trd lng="la">Dicotelydoneae</trd>), havantaj
      du <tld/>ojn.
    </dif>
  </snc>
  <trd lng="de">Zweikeimbl&auml;ttrige</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
