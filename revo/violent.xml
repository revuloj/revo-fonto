<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>violent</rad>/a</kap>
<drv mrk="violent.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Senbride perforta:
      <ekz>
        meti sur iun <tld/>ajn manojn;
      </ekz>
      <ekz>
        (analoge) <tld/>a morto.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">violento, -a</trd>
  <trd lng="pt">violento, -a</trd>
</drv>

<drv mrk="violent.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Uzo de la forto por trudi al iu kontra&ubreve;volan agon a&ubreve; sintenon.
    </dif>
  </snc>
  <trd lng="es">violencia</trd>
  <trd lng="pt">viol&ecirc;ncia</trd>
</drv>

<drv mrk="violent.0i">
  <kap><tld/>i</kap>
  <snc>
    <dif>
      Agi <tld/>e.
    </dif>
  </snc>
</drv>

</art>

