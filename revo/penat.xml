<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: penat.xml,v 1.2 2023/06/13 14:29:19 afido Exp $">
    <kap>
      <rad>penat</rad>/oj
    </kap>

<drv mrk="penat.0oj">
  <kap><tld/>oj</kap>
  <snc>
    <uzo tip="fak">REL</uzo>
    <dif>
      (&ccirc;e la romanoj) <ref tip="super" cel="di.dom0o">Domdio</ref>j de nutra&jcirc;provizo:
      <ekz>
        brulis <klr>[&#8230;]</klr> la sanktejo de Vesto kun la <tld/>oj de la roma popolo
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XLVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi transportis miajn <tld/>ojn <klr>(t.e. translo&gcirc;i&gcirc;is)</klr> sub la unuan arka&jcirc;on de la
        Pont-Royal
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="lar1.0o">laro</ref>
    <trd lng="be">&c_P;&c_e;&c_n;&c_a;&c_t;&c_y;</trd>
    <trd lng="de">Penaten</trd>
    <trd lng="en">Di Penates</trd>
    <trd lng="fr">P&eacute;nates</trd>
    <trd lng="lat"><klr>(Dii)</klr> Penates</trd>
    <trd lng="ru">&c_P;&c_je;&c_n;&c_a;&c_t;&c_y;</trd>
    <trd lng="uk">&c_P;&c_je;&c_n;&c_a;&c_t;&c_i;</trd>
  </snc>
</drv>

  </art>

