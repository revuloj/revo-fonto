<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ribek</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ribek.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Antikva mediteranea kaj e&ubreve;ropa <ref tip="super"
      cel="instru2.kord0o">kordinstrumento</ref> kun tri ar&ccirc;e sonigataj
      kordoj:
      <ekz>
        li scipovas legi kaj skribi kaj ludas belege la
        <tld/>on<fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro 11a, p.
        83a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">rebec</trd>
  <trd lng="fr">rebec</trd>
  <trd lng="hu">rebec</trd>
  <trd lng="ru">&c_r;&c_je;&c_b;&c_je;&c_k;</trd>
</drv>
</art>

