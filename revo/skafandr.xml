<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>skafandr</rad>/o
</kap>
<drv mrk="skafandr.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">MAR</uzo>
  <uzo tip="fak">AVI</uzo>
  <snc>
    <dif>
      <ref tip="vid" cel="hermet.0a">Hermetika</ref> vesto,
      regule provizata per fre&scirc;a aero, kaj plej ofte garnita
      per vitra kaj metala kapujo, per kiu oni povas iri kaj
      labori subakve, en kosmo, sur granda alto (en maldensa aero) ktp:
      <ekz><tld/>isto</ekz>.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>diving suit <klr>(subakve)</klr></trd>,
    <trd>space suit <klr>(en kosmo)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_k;&c_a;&c_f;&c_a;&c_n;&c_d;&c_r;</trd>
</drv>
</art>

