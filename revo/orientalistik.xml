<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: orientalistik.xml,v 1.6 2024/01/30 16:09:26 revo Exp $">
<kap>
  <rad>orientalistik</rad>/o <fnt><bib>Viki</bib></fnt>
</kap>
<drv mrk="orientalistik.0o">
  <kap><tld/>o</kap>
  <snc mrk="orientalistik.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Studo de popoloj, lingvoj, moroj de la orienta,
      t.e. azia parto de la malnova mondo,
      precipe de &gcirc;iaj okcidentaj kaj sudaj landoj; 
      <ref tip="sin" cel="orient1.0o.SCI">orientalismo</ref>:
      <ekz>
        la priantikva <tld/>o <klr>...</klr> denove pli 
        multe malfermi&gcirc;as al la menshistoria problemaro
        <fnt>
          <aut>W. von Soden, trad. M. Wolf</aut>,
          <vrk><url ref="http://www.sorbzilla.de/aorient/aorient.pdf">
          Enkonduko en la Antikvan Orienton</url></vrk>,
          <lok>[2010]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="orientalistik.0o.SIN">
    Pli maloftaj formoj estas
    <em>orientistiko</em>:
      <ekz>
        &scirc;i studis en Vieno orientistikon
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Barbara_Frischmuth">
          Barbara Frischmuth</url>, 2013-12-31</lok>
        </fnt>.
      </ekz>
    kaj <em>orientologio</em>:
    <ekz>
      en Peterburgo li instruis en la kursoj pri orientologio
      <fnt>
        <aut>A. K&uuml;nzli</aut>,
        <vrk><url ref="http://www.plansprachen.ch/Zamenhof_judademando.pdf">
        La juda demando &ccirc;e Zamenhof...</url></vrk>,
        <lok>2009</lok>
      </fnt>.
    </ekz>
  </rim>
  <trdgrp lng="be">
    <trd>&c_u;&c_s;&c_h;&c_o;&c_d;&c_a;&c_z;&c_n;&c_a;&c_w;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_a;&c_r;&c_y;&c_je;&c_n;&c_t;&c_a;&c_l;&c_ib;&c_s;&c_t;&c_y;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>oriental&iacute;stica</trd>,
    <trd>orientalisme</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Orientalistik</trd>,
    <trd>Orientwissenschaft</trd>
  </trdgrp>
  <trd lng="fr">orientalisme<klr> (scientifique)</klr></trd>
  <trdgrp lng="hu">
    <trd>orientalisztika</trd>,
    <trd>keletkutat&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">ori&euml;ntalisme</trd>
  <trd lng="pl">orientalistyka</trd>
  <trd lng="pt">orientalismo</trd>
  <trd lng="ro">studii  <ind>orientale</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_s;&c_t;&c_o;&c_k;&c_o;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_o;&c_r;&c_i;&c_je;&c_n;&c_t;&c_a;&c_l;&c_i;&c_s;&c_t;&c_i;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">orientalizmus</trd>
  <trdgrp lng="cs">
    <trd>orientalistika</trd>,
    <trd>orientalizmus</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4E1C;&#x65B9;&#x5F0F; <pr>d&#x14D;ngf&#x101;ngsh&#xEC;</pr></trd>,
    <trd>&#x6771;&#x65B9;&#x5F0F; <pr>d&#x14D;ngf&#x101;ngsh&#xEC;</pr></trd>,
    <trd>&#x4E9A;&#x6D32;&#x7814;&#x7A76; <pr>y&#xE0;zh&#x14D;uy&#xE1;nji&#x16B;</pr></trd>,
    <trd>&#x4E9E;&#x6D32;&#x7814;&#x7A76; <pr>y&#xE0;zh&#x14D;uy&#xE1;nji&#x16B;</pr></trd>,
    <trd>&#x4E1C;&#x65B9;&#x4E3B;&#x4E49; <pr>d&#x14D;ngf&#x101;ngzh&#x1D4;y&#xEC;</pr></trd>,
    <trd>&#x6771;&#x65B9;&#x4E3B;&#x7FA9; <pr>d&#x14D;ngf&#x101;ngzh&#x1D4;y&#xEC;</pr></trd>
  </trdgrp>
</drv>
