<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hers.xml,v 1.13 2021/05/30 10:28:20 revo Exp $">
<kap>
  <rad>hers</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="hers.0o">
  <kap><tld/>o</kap>
  <snc mrk="hers.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Vertikale mallevebla <ref tip="super" cel="plat.0o">plato</ref>
      a&ubreve; <ref tip="super" cel="krad.0o.KOMUNE">krado</ref>, per kiu oni povas bari
      pasejon, ekzemple pordon de <ref tip="malprt" cel="fortik.0ajxo">
      fortika&jcirc;o</ref> a&ubreve; <ref tip="malprt" cel="kluz.0o.MAR">kluzon</ref>:
      <ekz>
        kastelo kun profunda foso, turoj, <tld/>o kaj levoponto
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 71a, p.
        797a</lok></fnt>;
      </ekz>
      <ekz>
        post la <tld/>o, oni eniras en la birdokorton kun dekstraflanke la &ccirc;evalejo
        <fnt>
          <vrk><url ref="https://esperanto-france.org/IMG/pdf/hk-plaquette_esperanto.pdf">La resta&ubreve;rado Arkitekturo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="klap.0o.pordo">klapo</ref>
    </refgrp>
    <bld
      lok="&WCU;/thumb/c/c9/Cahir_Castle_Portcullis_by_Kevin_King.jpg/640px-Cahir_Castle_Portcullis_by_Kevin_King.jpg"
      prm="CC BY 2.0"
      ><tld/>o de kastelo Cahir, Irlando
        <fnt>
          <aut>Kevin King el Pensacola, FL, US; al&scirc;utita de guillom</aut>
          <lok><url ref="&WCW;/File:Cahir_Castle_Portcullis_by_Kevin_King.jpg"
              >https://commons.wikimedia.org/wiki/File:Cahir_Castle_Portcullis_by_Kevin_King.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="ar">&a_b;&a_A;&a_b; &a_m;&a_n;&a_z;&a_l;&a_q;</trd>
  <trdgrp lng="cs">
    <trd>padac&iacute; br&aacute;na</trd>,
    <trd>padac&iacute; m&rcaron;&iacute;&zcaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Fallgatter</trd>,
    <trd>Fallgitter</trd>
  </trdgrp>
  <trd lng="en">portcullis</trd>
  <trd lng="es">rastrillo</trd>
  <trd lng="fr">herse</trd>
  <trd lng="he">&shin;&ayin;&resh; &mem;&tav;&resh;&vav;&mem;&fmem;</trd>
  <trdgrp lng="hu">
    <trd>zsilip</trd>,
    <trd>csap&oacute;r&aacute;cs</trd>,
    <trd>kapurost&eacute;ly</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>chiudenda<klr> (fortificazione)</klr></trd>,
    <trd>saracinesca<klr> (fortificazione)</klr></trd>
  </trdgrp>
  <trd lng="nl">valhek</trd>
  <trdgrp lng="pl">
    <trd>brona</trd>,
    <trd>krata<klr> forteczna</klr></trd>,
    <trd>brama zwodzona</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>grade</trd>,
    <trd>rastelo</trd>
  </trdgrp>
  <trd lng="ru">
  &c_p;&c_o;&c_d;&c_malmol;&c_jo;&c_m;&c_n;&c_a;&c_ja;
  &c_r;&c_je;&c_sh;&c_jo;&c_t;&c_k;&c_a;
  </trd>
  <trdgrp lng="sk">
    <trd>mre&zcaron;a</trd>,
    <trd>padacia br&aacute;na</trd>
  </trdgrp>
  <trd lng="tr">dikenli parmakl&inodot;k</trd>
</drv>
</art>
