<vortaro>
<art mrk="$Id$">
<kap>
    <rad>ornitorinked</rad>/oj
</kap>
<drv mrk="ornito2.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="ornito2.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" 
      lst="voko:zoologiaj_familioj">Familio</ref>
      de <ref tip="malprt" cel="mam.pra0uloj">pramamuloj</ref>
      (<trd lng="la">Ornithorhynchidae</trd>)
      kun la sola ankora&ubreve; vivanta specio
      <ref tip="prt" cel="ornito1.0o">ornitorinko</ref>.
    </dif>
  </snc>
  <trd lng="cs">ptakopyskovití</trd>
  <trd lng="de">Schnabeltiere</trd>
  <trd lng="sk">vtákopyskovité</trd>
</drv>
</art>

