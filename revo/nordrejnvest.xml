<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
   <kap>
    <rad>Nordrejn-Vestfali</rad>/o
   </kap>
   <drv mrk="nordrejnvest.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">GEOG</uzo>
    <snc>
       <dif>
      La plej lo&gcirc;antori&ccirc;a el la federaciaj &scirc;tatoj de
      <ref tip="malprt" cel="german1.G0ujo">Germanujo</ref>, situanta en la
plej okcidenta parto kaj trafluata de la rivero <ref tip="vid"
cel="rejn.0o">Rejno</ref>.
      Regurbo estas Duseldorfo, plej granda urbo estas <ref tip="vid"
cel="kolonj.0o">Kolonjo</ref>.
       </dif>
       <trd lng="de">Nordrhein-Westfalen</trd>
       <trd lng="hu">&Eacute;szak-Rajna-Vesztf&aacute;lia</trd>
       <trd lng="pt">Ren&acirc;nia do Norte-Vestef&aacute;lia</trd>
    </snc>
</drv>

  <drv mrk="nordrejnvest.0a">
    <kap><tld lit="n"/>a</kap>
    <snc>
      <dif>
         Rilatanta al Nordrejn-Vestfalio.
      </dif>
    </snc>
    <trdgrp lng="de">
       <trd>nordrhein-westf&auml;lisch</trd>
    </trdgrp>
    <trd lng="hu">&eacute;szak-rajna-vesztf&aacute;liai</trd>    
  </drv>

  </art>

