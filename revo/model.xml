    <trd>&c_v;&c_i;&c_g;&c_o;&c_t;&c_o;&c_v;&c_l;&c_ja;&c_t;&c_i; &c_z;&c_a; &c_m;&c_o;&c_d;&c_je;&c_l;&c_l;&c_ju;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_l;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="model.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="model.0igi.KOMUNE">
    <dif>
      Reprodukti la&ubreve; <ref cel="model.0o.KOMUNE">modelo<sncref/></ref> a&ubreve;
      <ref cel="model.0o.FIG"><sncref/></ref>;
      adapti, konformigi al <tld/>o:
