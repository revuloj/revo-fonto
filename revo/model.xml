<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>*</ofc>
    <rad>model</rad>/o
</kap>
<drv mrk="model.0o">
    <kap><ofc>*</ofc><tld/>o</kap>
    <snc>
      <dif>
        Objekto farita por esti imitata:
        <ekz>
          <tld/>o de statuo, de vazo, de broda&jcirc;o, de skribo.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>Modell</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>minta</trd>,
        <trd>&ouml;nt&odblac;minta</trd>
        <trd>mintadarab</trd>
      </trdgrp>
    </snc>
    <snc>
      <uzo tip="fak">BELA</uzo>
      <dif>
        Persono, kiu pozas anta&ubreve; artisto por esti desegne, pentre,
skulpte
        ks reprezentata; pozisto.
      </dif>
      <trd lng="hu">modell</trd>      
      <trdgrp lng="de">
        <trd>Modell</trd>
      </trdgrp>
    </snc>
    <snc>
      <uzo tip="stl">FIG</uzo>
      <dif>
        Io imitinda pro sia perfekteco:
        <ekz>
          tiu aparato estas <tld/>o de simpleco;
        </ekz>
        <ekz>
          tiu vesto estas <tld/>o de bongusto;
        </ekz>
        <ekz>
          &scirc;i liveras <tld/>on de patrina amo
        </ekz>
      </dif>
      <trdgrp lng="hu">
        <trd>minta</trd>,
        <trd>mintak&eacute;p</trd>,
        <trd>p&eacute;ldak&eacute;p</trd>
      </trdgrp>
      <refgrp tip="sin">
        <ref cel="tip.0o">tipo</ref>,
        <ref cel="ekzemp.0o">ekzemplo</ref>,
        <ref cel="ideal.0o">idealo</ref>.
      </refgrp>
      <trdgrp lng="de">
        <trd>Modell</trd>
      </trdgrp>
    </snc>
    <trdgrp lng="be">
      <trd>&c_m;&c_a;&c_d;&c_e;&c_l;&c_mol;</trd>,
      <trd>&c_u;&c_z;&c_o;&c_r;</trd>
    </trdgrp>
    <trd lng="cs">vzor</trd>
    <trd lng="nl">model</trd>
    <trd lng="fr">mod&egrave;le</trd>
    <trdgrp lng="ru">
      <trd>&c_m;&c_o;&c_d;&c_je;&c_l;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_r;&c_a;&c_z;&c_je;&c_c;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Modell</trd>
    </trdgrp>
</drv>

<drv mrk="model.0a">
    <kap><tld/>a</kap>
    <snc>
      <dif>
        Inda servi kiel <tld/>o:
        <ekz>
          <tld/>a konduto, stilo;
        </ekz>
        <ekz>
          <tld/>e plenumi sian taskon.
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="be">
      <trd>&c_m;&c_a;&c_d;&c_e;&c_l;&c_mol;&c_n;&c_y;</trd>,
      <trd>&c_u;&c_z;&c_o;&c_r;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="nl">voorbeeldig</trd>
    <trdgrp lng="ru">
      <trd>&c_m;&c_o;&c_d;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_o;&c_b;&c_r;&c_a;&c_z;&c_c;&c_o;&c_v;&c_y;&c_j;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>mod&egrave;le</trd>,
      <trd>exemplaire</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>minta-</trd>,
      <trd>mintaszer&udblac;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>modellhaft</trd>
    </trdgrp>
</drv>

<drv mrk="model.0i">
    <kap><tld/>i</kap>
    <gra><vspec>tr</vspec></gra>
    <snc>
      <dif>
        Fari reproduktotan <tld/>on:
        <ekz>
          <tld/>i buston
        </ekz>
      </dif>
      <refgrp tip="sin">
        <ref cel="modl.0i">modli</ref>.
      </refgrp>
    </snc>
  <trdgrp lng="hu">
    <trd>mint&aacute;z</trd>,
    <trd>megmint&aacute;z</trd>,
    <trd>alak&iacute;t</trd>,
    <trd>modell&aacute;l</trd>,
    <trd>mint&aacute;t k&eacute;sz&iacute;t</trd>
  </trdgrp>
    <snc>
      <dif>
        Reprodukti <tld/>on ver&scirc;ante a&ubreve; premante moligitan
materion en
        formilon:
        <ekz>
          <tld/>i bronzan statueton
        </ekz>
      </dif>
      <refgrp tip="sin">
        <ref cel="muld.0i">muldi</ref>.
      </refgrp>
      <trd lng="hu">ki&ouml;nt<klr tip="amb">(form&aacute;ba)</klr></trd>

    </snc>
    <trdgrp lng="be">
      <trd>&c_m;&c_a;&c_d;&c_e;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; 
<ind>&c_m;&c_a;&c_d;&c_e;&c_l;&c_mol;</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>megmint&aacute;z</trd>,
      <trd>modell&aacute;l</trd>
    </trdgrp>    
    <trd lng="nl">modelleren</trd>
    <trd lng="de">modellieren</trd>
    <trd 
lng="ru">&c_i;&c_z;&c_g;&c_o;&c_t;&c_o;&c_v;&c_l;&c_ja;&c_t;&c_mol; 
&c_m;&c_o;&c_d;&c_je;&c_l;&c_mol;</trd>
</drv>

<drv mrk="model.0ilo">
    <kap><tld/>ilo</kap>
    <snc>
      <dif>
        Formilo, muldilo
      </dif>
      <refgrp tip="sin">
        <ref cel="sxablo.0o">&scirc;ablono</ref>,
        <ref cel="matric.0o">matrico</ref>.
      </refgrp>
    </snc>
    <trdgrp lng="hu">
      <trd>forma</trd>,
      <trd>&ouml;nt&odblac;forma</trd>
    </trdgrp>    
    <trdgrp lng="nl">
      <trd>gietvorm</trd>,
      <trd>mal</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Modell</trd>,
      <trd>Gussform</trd>,
      <trd>Schablone</trd>
    </trdgrp>
</drv>

<drv mrk="model.0igi">
    <kap><tld/>igi</kap>
    <snc>
      <dif>
        Konformigi al <tld/>o:
        <ekz>
          estus la plej bone, se niaj amikoj &ccirc;iam <tld/>igadus sian
stilon
          la&ubreve; la stilo de la gazeto
&leftquot;Esperantisto&rightquot;.<fnt>Z</fnt>.
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="hu">
      <trd>p&eacute;ld&aacute;t vesz</trd>,
      <trd>mint&aacute;t vesz</trd>
    </trdgrp>
    <trd lng="nl">richten naar</trd>
    <trd lng="de">sich richten nach</trd>
    <trd lng="fr">conformer &agrave; un <ind>mod&egrave;le</ind></trd>
</drv>
</art>
