<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>model</rad>/o
</kap>
<drv mrk="model.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="model.0o.KOMUNE">
    <dif>
      Objekto farita por esti imitata:
      <ekz>
        li ekzercis la literojn, kaj ili staris tiel rekte kaj sane, kiel
        povas stari nur skrib<tld/>o
        <fnt><bib>Fab2</bib>, <lok>dio de dormo, Lundo</lok></fnt>;
      </ekz>
      <ekz>
        mi eldonis la Fundamentan Krestomation, kiu povos servi al &ccirc;iuj
        kiel <tld/>o de esperanta stilo
        <fnt><bib>FK</bib>, <lok>anta&ubreve;parolo</lok></fnt>;
      </ekz>
      <ekz>
        pasante li ekrigardis la <tld/>on de braceleto, desegnitan de Marta
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>minta</trd>,
      <trd>&ouml;nt&odblac;minta</trd>,
      <trd>mintadarab</trd>
    </trdgrp>
  </snc>
  <snc mrk="model.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Io imitinda pro sia perfekteco:
      <ekz>
        tiu aparato estas <tld/>o de simpleco;
      </ekz>
      <ekz>
        tiu vesto estas <tld/>o de bongusto;
      </ekz>
      <ekz>
        jen estas mia frato, <tld/>o de &ccirc;iuj ecoj kaj perfekteco, knabo
        bona
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>minta</trd>,
      <trd>mintak&eacute;p</trd>,
      <trd>p&eacute;ldak&eacute;p</trd>
    </trdgrp>
    <refgrp tip="vid">
      <ref cel="ekzemp.0o">ekzemplo</ref>,
      <ref cel="ideal.0o">idealo</ref>,
      <ref cel="tip.0o">tipo</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Modell</trd>
    </trdgrp>
  </snc>
  <snc mrk="model.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <ref tip="dif" cel="poz.0isto">pozisto</ref>
    <trd lng="hu">modell</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_d;&c_e;&c_l;&c_mol;</trd>,
    <trd>&c_u;&c_z;&c_o;&c_r;</trd>
  </trdgrp>
  <trd lng="cs">vzor</trd>
  <trdgrp lng="de">
    <trd>Modell</trd>
  </trdgrp>
  <trd lng="fr">mod&egrave;le</trd>
  <trd lng="nl">model</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_d;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_r;&c_a;&c_z;&c_je;&c_c;</trd>,
    <trd>&c_m;&c_a;&c_k;&c_je;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="model.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Inda servi kiel <tld/>o:
      <ekz>
        &ccirc;io alia, kion mi verkis <klr>...</klr> la esperantistoj
        <klr>...</klr> povas rigardadi kiel <tld/>a, sed ne kiel deviga
        <fnt><bib>F</bib>, <lok>anta&ubreve;parolo</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a konduto, stilo;
      </ekz>
      <ekz>
        <tld/>e plenumi sian taskon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_d;&c_e;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_u;&c_z;&c_o;&c_r;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>modellhaft</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>mod&egrave;le<klr tip="amb"> (adj.)</klr></trd>,
    <trd>exemplaire</trd>
  </trdgrp>
  <trd lng="nl">voorbeeldig</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_d;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_b;&c_r;&c_a;&c_z;&c_c;&c_o;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>minta-</trd>,
    <trd>mintaszer&udblac;</trd>
  </trdgrp>
</drv>

<drv mrk="model.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari reproduktotan <tld/>on:
      <ekz>
        <tld/>i buston.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="muld.0i">muldi</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="modl.0i">modli</ref>
    </refgrp>
    <trdgrp lng="hu">
      <trd>mint&aacute;z</trd>,
      <trd>megmint&aacute;z</trd>,
      <trd>alak&iacute;t</trd>,
      <trd>modell&aacute;l</trd>,
      <trd>mint&aacute;t k&eacute;sz&iacute;t</trd>
      <trd>ki&ouml;nt<klr tip="amb">(form&aacute;ba)</klr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_d;&c_e;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
    <ind>&c_m;&c_a;&c_d;&c_e;&c_l;&c_mol;</ind></trd>
  </trdgrp>
  <trd lng="de">modellieren</trd>
  <trdgrp lng="hu">
    <trd>megmint&aacute;z</trd>,
    <trd>modell&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">modelleren</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_d;&c_je;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; <ind>&c_m;&c_o;&c_d;&c_je;&c_l;&c_mol;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="model.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Formilo, muldilo.
    </dif>
    <refgrp tip="vid">
      <ref cel="matric.0o">matrico</ref>,
      <ref cel="sxablo.0o">&scirc;ablono</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Modell</trd>,
    <trd>Gussform</trd>,
    <trd>Schablone</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>forma</trd>,
    <trd>&ouml;nt&odblac;forma</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gietvorm</trd>,
    <trd>mal</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_d;&c_je;&c_l;&c_mol;<klr tip="amb"> (&c_v; &c_l;&c_i;&c_t;&c_je;&c_j;&c_n;&c_o;&c_m; &c_d;&c_je;&c_l;&c_je;)</klr></trd>,
    <trd>&c_sh;&c_a;&c_b;&c_l;&c_o;&c_n;</trd>
  </trdgrp>
</drv>

<drv mrk="model.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Konformigi al <tld/>o:
      <ekz>
        estus la plej bone, se niaj amikoj &ccirc;iam <tld/>igadus sian
        stilon la&ubreve; la stilo de la gazeto &leftquot;Esperantisto&rightquot;.<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">sich richten nach</trd>
  <trd lng="fr">conformer &agrave; un <ind>mod&egrave;le</ind></trd>
  <trdgrp lng="hu">
    <trd>p&eacute;ld&aacute;t vesz</trd>,
    <trd>mint&aacute;t vesz</trd>
  </trdgrp>
  <trd lng="nl">richten naar</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_a;&c_t;&c_mol; <ind>&c_p;&c_r;&c_i;&c_m;&c_je;&c_r;</ind></trd>,
    <trd>&c_b;&c_r;&c_a;&c_t;&c_mol; &c_z;&c_a; <ind>&c_o;&c_b;&c_r;&c_a;&c_z;&c_je;&c_c;</ind></trd>
  </trdgrp>
</drv>
</art>
