<drv mrk="model.0i">
  <kap><tld/>i</kap>

  <snc mrk="model.0i.servi">
    <gra><vspec>ntr</vspec></gra>
    <dif>
       Servi kiel <ref cel="model.0o.FIG">modelo<sncref/></ref>:
      <ekz>
        &gcirc;i mesa&gcirc;as pacan kaj bonan kunvivadon kaj najbarecon, kaj
        <tld/>as kiel komunikponto inter amba&ubreve; nacioj
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2008/009618.php">Kunlaboro
          mielmonata</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la francoj multe <tld/>is la urbon dum sia rego per sia propra
        arkitektura stilo, nome bulvardoj flankitaj de arboj kaj multaj koloniaj
        konstrua&jcirc;oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=61104">IJK
          2007</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>

  <snc mrk="model.0i.KOMUNE">
    <gra><vspec>tr</vspec></gra>
    <dif>
      Fari reproduktotan <tld/>on:
      <ekz>
        <tld/>i buston.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="modl.0i">modli</ref>,
      <ref cel="muld.0i">muldi</ref>
    </refgrp>
    <trd lng="de">modellieren</trd>
    <trd lng="en">model</trd>
    <trdgrp lng="hu">
      <trd>megmint&aacute;z</trd>,
      <trd>modell&aacute;l</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>mint&aacute;z</trd>,
      <trd>megmint&aacute;z</trd>,
      <trd>alak&iacute;t</trd>,
      <trd>modell&aacute;l</trd>,
      <trd>mint&aacute;t k&eacute;sz&iacute;t</trd>
      <trd>ki&ouml;nt<klr tip="amb">(form&aacute;ba)</klr></trd>
    </trdgrp>
    <trd lng="nl">modelleren</trd>
    <trdgrp lng="ru">
      <trd>&c_m;&c_o;&c_d;&c_je;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; <ind>&c_m;&c_o;&c_d;&c_je;&c_l;&c_mol;</ind></trd>
    </trdgrp>
  </snc>

  <snc mrk="model.0i.konformigi">
    <gra><vspec>tr</vspec></gra>
    <dif>
      <ref cel="model.0igi.KOMUNE"><tld lit="M"/>igi<sncref/></ref>:
      <ekz>
        la tepoto de Utaho estas kvaza&ubreve;norma referenca objekto de
        komputila grafiko,
        <klr>[&#x2026;]</klr> difinita de <nom>Martin Newell</nom>, kiu <tld/>is &gcirc;in per
        bezieraj surfaceroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=104503">Tekru&ccirc;o
          de Utaho</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">modellieren</trd>
  </snc>

  <snc mrk="model.0i.SCI">
    <gra><vspec>tr</vspec></gra>
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="dif" cel="model.0umi.SCI"><tld lit="M"/>umi</ref>:
      <ekz>
        helpe de komputila programo ili <tld/>is 1936 eblajn
        konfigurojn, pruvis,
        ke tiuj &ccirc;i konfiguroj kovras &ccirc;iujn eblecojn, kaj montris &ccirc;e &ccirc;iu
        el ili,
        ke por kolorigi &gcirc;in sufi&ccirc;as kvar koloroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=217444">Kvarkolormapa
          teoremo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        sukeraj solva&jcirc;oj <tld/>as fiziologiajn likva&jcirc;ojn
        <fnt>
          <aut>trad. M. M. Mahnac</aut>,
          <vrk><url ref="https://web.archive.org/web/20070519162841/http://www.smolensk.ru:80/user/sgma/MMORPH/N-11-html/esp-11-html.htm">
          Dependeco De Optika Aktiveco De Sukero De Eksteraj
          Kondicoj</url></vrk>,
          <lok>[2009?]</lok>
        </fnt>;
      </ekz>
      <ekz>
        por <tld/>i la Esperantan gramatikon estas skribita kaj uzita
        parsalgoritmo
        <fnt>
          <aut>S. Ben-Avi</aut>,
          <vrk><url ref="http://www.edukado.net/biblioteko/diplomlaborajhoj?iid=46">Esploro
          pri la uzo de Esperanto kiel
          interlingvo...</url></vrk>,
          <lok>[vidita en 2011]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">modellieren</trd>
    <trd lng="fr">mod&eacute;liser</trd>
    <trdgrp lng="zh">
    <trd>&#x505A;&#x6A21;&#x578B; <pr>zu&#xF2;m&#xF3;x&#xED;ng</pr></trd>,
    <trd>&#x6A21;&#x5851; <pr>m&#xF3;s&#xF9;</pr></trd>
  </trdgrp>
</snc>
    <trdgrp lng="be">
      <trd>&c_m;&c_a;&c_d;&c_e;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_m;&c_a;&c_d;&c_e;&c_l;&c_mol;</ind></trd>,
      <trd>&c_s;&c_l;&c_u;&c_zh;&c_y;&c_c;&c_mol; <ind>&c_m;&c_a;&c_d;&c_e;&c_l;&c_mol;&c_l;&c_ju;</ind></trd>
    </trdgrp>
  <trdgrp lng="cs">
    <trd>modelovat</trd>,
    <trd>tvo&rcaron;it modely</trd>
  </trdgrp>
  <trdgrp lng="ja">
