    <pr>&#x3072;&#x306A;&#x304C;&#x305F;&#x3092;&#x3064;&#x304F;&#x308B;</pr></trd>,
    <trd>&#x578B;&#x306B;&#x5165;&#x308C;&#x3066;&#x4F5C;&#x308B; 
    <pr>&#x304B;&#x305F;&#x306B;&#x3044;&#x308C;&#x3066;&#x3064;&#x304F;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
