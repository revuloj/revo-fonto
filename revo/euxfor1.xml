<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: euxfor1.xml,v 1.17 2022/03/17 13:47:19 revo Exp $">
<kap>
  <rad>e&ubreve;forbiac</rad>/oj <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="euxfor1.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="euxfor1.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref>  de <ref tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>,
      unu- a&ubreve; senpetalaj plantoj (<trd lng="la">Euphorbiaceae</trd>):
      <ekz>
        <tld/>oj estas plantfamilio kun pli ol 8000 specioj, 300 genroj al kiuj
        apartenas i.a. la <ref tip="prt" cel="maniok.0o">manioko</ref>
        (donas tapiokon), <ref tip="prt" cel="euxfor.0o">e&ubreve;forbio</ref>, ka&ubreve;&ccirc;ukarbo (donas
        ka&ubreve;&ccirc;ukon), <ref tip="prt" cel="ricin.0o">ricino</ref>, 
        kaj <ref tip="prt" cel="merkur.0o">merkurialo</ref>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=20145">E&ubreve;forbio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_a;&c_ch;&c_a;&c_j;&c_n;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">euforbi&agrave;cies</trd>
  <trd lng="de">Wolfsmilchgew&auml;chse</trd>
  <trd lng="fr">euphorbiac&eacute;es</trd>
  <trd lng="hu">kutyatejf&eacute;l&eacute;k</trd>
  <trd lng="pl">wilczomleczowate</trd>
  <trd lng="pt">euforbi&aacute;ceas</trd>
  <trd lng="ru">&c_m;&c_o;&c_l;&c_o;&c_ch;&c_a;&c_j;&c_n;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&#x43C;&#x43E;&#x43B;&#x43E;&#x447;&#x430;&#x439;&#x43D;&#x456;</trd>
</drv>
</art>
