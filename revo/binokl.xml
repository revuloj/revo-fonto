<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>binokl</rad>/o
</kap>
<drv mrk="binokl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      Malgranda dutuba <ref tip="super" cel="lorn.0o">lorno</ref>,
      uzata en teatro a&ubreve; dum ekskursoj.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="lorn.du0eto">dulorneto</ref>
  </refgrp>
  <trdgrp lng="es">
    <trd>gemelos</trd>,
    <trd>prism&aacute;ticos</trd>,
    <trd>binoculares</trd>
  </trdgrp>
  <trd lng="fr">jumelles <klr>(optique)</klr></trd>
  <trd lng="hu">binokul&aacute;r</trd>
  <trd lng="ru">&c_b;&c_i;&c_n;&c_o;&c_k;&c_l;&c_mol;</trd>
</drv>
</art>
