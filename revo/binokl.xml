<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: binokl.xml,v 1.19 2021/03/04 20:39:52 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>binokl</rad>/o
</kap>

<drv mrk="binokl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      Malgranda dutuba <ref tip="super" cel="lorn.0o">lorno</ref>,
      uzata en teatro a&ubreve; dum ekskursoj:
      <ekz>
        kredu, ke mi vidas lin per ... <tld/>o tre detale
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="http://steloj.de/esperanto/butiko">La Stranga Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        birdorigardado <klr>[&#x2026;]</klr> per vida plibonigo danke al uzado de <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=212108">Birdumo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="lorn.du0eto">dulorneto</ref>
  </refgrp>
  <trd lng="be">&c_b;&c_ib;&c_n;&c_o;&c_k;&c_l;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>Binokular</trd>,
    <trd>Feldstecher</trd>,
    <trd>Opernglas</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>gemelos</trd>,
    <trd>prism&aacute;ticos</trd>,
    <trd>binoculares</trd>
  </trdgrp>
  <trd lng="en">binoculars</trd>
  <trd lng="fr">jumelles <klr>(optique)</klr></trd>
  <trd lng="he">&#x5DE;&#x5B4;&#x5E9;&#x5C1;&#x5E7;&#x5B6;&#x5E4;&#x5B6;&#x5EA;</trd>
  <trdgrp lng="hu">
    <trd>l&aacute;tcs&odblac;</trd>,
    <trd>binokul&aacute;r</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>toneelkijker</trd>,
    <trd>verrekijker</trd>
  </trdgrp>
  <trd lng="pl">lornetka</trd>
  <trd lng="ru">&c_b;&c_i;&c_n;&c_o;&c_k;&c_l;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>binokel</trd>,
    <trd>mal&yacute; &dcaron;alekoh&lcaron;ad</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>binokl</trd>,
    <trd>kuk&aacute;tko</trd>,
    <trd>mal&yacute; dalekohled</trd>,
    <trd>triedr</trd>
  </trdgrp>
</drv>
</art>
