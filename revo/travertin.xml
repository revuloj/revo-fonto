<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: travertin.xml,v 1.8 2022/05/05 17:08:11 revo Exp $">
<kap>
  <rad>travertin</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="travertin.0o">
  <kap><tld/>o</kap>
  <snc mrk="travertin.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      <ref tip="dif" cel="tof.kalko0o.MIN">Kalkotofo</ref>:
      <ekz>
        en valo trovi&gcirc;as hele flava <tld/>o sur tera surfaco
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url ref="&CRI;221/2008/11/11/1s88559.htm">
          Huanglong, unu el mondaj naturaj hereda&jcirc;oj</url></vrk>,
          <lok>2008-11-11</lok>
        </fnt>;
      </ekz>
      <ekz>
        grotaj &scirc;tonoj, formi&gcirc;intaj sur la <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=499067">Regionhistoria muzeo de Greu&szlig;en</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">travertin</trd>
  <trdgrp lng="de">
    <trd>Travertin</trd>,
    <trd>Kalktuff</trd>
  </trdgrp>
  <trd lng="it">travertino</trd>
  <trd lng="pl">trawertyn</trd>
  <trd lng="ro">travertin</trd>
  <trd lng="sk">travert&iacute;n</trd>
  <trdgrp lng="uk">
    <trd>&#x442;&#x440;&#x430;&#x432;&#x435;&#x440;&#x442;&#x438;&#x43D;</trd>,
    <trd>&#x432;&#x430;&#x43F;&#x43D;&#x44F;&#x43A;&#x43E;&#x432;&#x438;&#x439; &#x442;&#x443;&#x444;</trd>
  </trdgrp>
</drv>
</art>

