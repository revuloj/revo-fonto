<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: travertin.xml,v 1.11 2024/05/10 10:43:46 revo Exp $">
<kap>
  <rad>travertin</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="travertin.0o">
  <kap><tld/>o</kap>
  <snc mrk="travertin.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      <ref tip="dif" cel="tof.kalko0o.MIN">Kalkotofo</ref>, precipe la malmola polurebla speco:
      <ekz>
        en valo trovi&gcirc;as hele flava <tld/>o sur tera surfaco
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url ref="&CRI;221/2008/11/11/1s88559.htm">
          Huanglong, unu el mondaj naturaj hereda&jcirc;oj</url></vrk>,
          <lok>2008-11-11</lok>
        </fnt>;
      </ekz>
      <ekz>
        grotaj &scirc;tonoj, formi&gcirc;intaj sur la <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=499067">Regionhistoria muzeo de
          Greu&szlig;en</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_a;&c_v;&c_e;&c_r;&c_t;&c_y;&c_n;</trd>,
    <trd>&c_v;&c_a;&c_p;&c_n;&c_ja;&c_k;&c_o;&c_v;&c_y; <ind>&c_t;&c_u;&c_f;</ind></trd>
  </trdgrp>
  <trd lng="cs">travertin</trd>
  <trdgrp lng="de">
    <trd>Travertin</trd>,
    <trd>Kalktuff</trd>
  </trdgrp>
  <trd lng="it">travertino</trd>
  <trd lng="pl">trawertyn</trd>
  <trd lng="ro">travertin</trd>
  <trd lng="sk">travert&iacute;n</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_r;&c_a;&c_v;&c_je;&c_r;&c_t;&c_i;&c_n;</trd>,
    <trd>&c_v;&c_a;&c_p;&c_n;&c_ja;&c_k;&c_o;&c_v;&c_i;&c_j; &c_t;&c_u;&c_f;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x77F3;&#x7070;&#x534E; <pr>sh&#xED;hu&#x12B;hu&#xE1;</pr></trd>,
    <trd>&#x77F3;&#x7070;&#x83EF; <pr>sh&#xED;hu&#x12B;hu&#xE1;</pr></trd>,
    <trd>&#x6D1E;&#x77F3; <pr>d&#xF2;ngsh&#xED;</pr></trd>
  </trdgrp>
</drv>
</art>

