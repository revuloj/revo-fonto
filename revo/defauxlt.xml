  <trd lng="ru">&c_u;&c_m;&c_o;&c_l;&c_ch;&c_a;&c_n;&c_i;&c_je;t<klr> (&c_z;&c_n;&c_a;&c_ch;&c_je;&c_n;&c_i;&c_je; &c_p;&c_o; &c_u;&c_m;&c_o;&c_l;&c_ch;&c_a;&c_n;&c_i;&c_ju;)</klr></trd>
