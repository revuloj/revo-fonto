<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vila&gcirc;</rad>/o
</kap>

<drv mrk="vilagx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vilagx.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Malgranda aro da domoj en kamparo, montaro...:
      <ekz>
        alveni rekte el sia <tld/>o
        <klr>(esti krude nesperta)</klr>;
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_jo;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_ja;&c_l;&c_o;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_mol;&c_c;&c_je;&c_n;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_je;&c_l;&c_o;</trd>
  <trd lng="ca">poble</trd>
  <trd lng="cs">vesnice</trd>
  <trd lng="de">Dorf</trd>
  <trd lng="en">village</trd>
  <trdgrp lng="es">
    <trd>pueblo</trd>,
    <trd>villa</trd>
  </trdgrp>
  <trd lng="fr">village</trd>
  <trdgrp lng="id">
    <trd>desa</trd>,
    <trd>dusun</trd>,
    <trd>kampung</trd>
  </trdgrp>
  <trd lng="it">villaggio</trd>
  <trd lng="nl">dorp</trd>
  <trd lng="hu">falu</trd>
  <trd lng="pl">wie&sacute;</trd>
  <trdgrp lng="pt">
    <trd>vila</trd>,
    <trd>aldeia</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_r;&c_je;&c_v;&c_n;&c_ja;</trd>,
    <trd>&c_s;&c_je;&c_l;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">dedina</trd>
  <trd lng="uk">&c_s;&c_je;&c_l;&c_o;</trd>
</drv>

<drv mrk="vilagx.0ano">
  <kap><tld/>ano</kap>
  <snc mrk="vilagx.0ano.KOMUNE">
    <dif>
      Lo&gcirc;anto en vila&gcirc;o:
      <ekz>
        virino <klr>...</klr> vestita en ordinara vesto de rusa
        <tld/>anino
        <fnt><bib>Voja&gcirc;Imp</bib>, <lok>du blinduloj, sceno el la lando
        de
        Donaj kozakoj</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kamp.0arano.AGR">kamparano</ref>
    </refgrp>
  </snc>
  <trd lng="bg">&c_s;&c_je;&c_l;&c_ja;&c_n;&c_i;&c_n;</trd>
  <trd lng="ca">poblet&agrave;</trd>
  <trd lng="de">Dorfbewohner</trd>
  <trdgrp lng="es">
    <trd>aldeano</trd>,
    <trd>lugare&ntilde;o</trd>
  </trdgrp>
  <trd lng="fr">villageois</trd>
  <trd lng="hu">falusi</trd>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_s;&c_t;&c_mol;&c_ja;&c_n;&c_i;&c_n;</trd>
</drv>
</art>
