<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>profesi</rad>/o
</kap>
<drv mrk="profes.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;iu okupo, per kiu oni akiras vivrimedojn:
      <ekz>
        metia, libera <tld/>o;
      </ekz>
      <ekz>
        <tld/>a oratoro;
      </ekz>
      <ekz>
        <tld/>a sekreto.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="meti.0o">metio</ref>,
      <ref cel="okup.0o">okupo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Beruf</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_f;&c_je;&c_s;&c_s;&c_i;&c_ja;</trd>
  <trd lng="sv">yrke</trd>
</drv>

<drv mrk="profes.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Praktiki <tld/>on.
    </dif>
  </snc>
  <trd lng="de">praktizieren</trd>
</drv>

<drv mrk="profes.0isto">
  <kap><tld/>isto, <tld/>ulo</kap>
  <snc>
    <dif>
      Homo, kiu praktikas okupon por gajni vivpanon.
    </dif>
    <ref tip="ant" cel="amator.0o">amatoro</ref>
  </snc>
  <trd lng="de">Professioneller</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_f;&c_je;&c_s;&c_s;&c_i;&c_o;&c_n;&c_a;&c_l;</trd>
</drv>
</art>
