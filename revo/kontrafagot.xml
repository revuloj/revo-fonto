<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kontrafagot.xml,v 1.6 2023/04/27 17:25:36 revo Exp $">
<kap>
  <rad>kontrafagot</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kontrafagot.0o">
  <kap><tld/>o</kap>
  <snc mrk="kontrafagot.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="lst"
      cel="instru2.blov0o"
      lst="voko:blovinstrumentoj">Blovinstrumento</ref>,
      basa <ref tip="vid" cel="fagot.0o.MUZ">fagoto</ref>
      kun longa, plurfoje kurbigita tubo:
      <ekz>
        la &hcirc;oron &ccirc;i tie akompanas fagoto, <tld/>o,
        trumpeto kaj kornoj
        <fnt>
          <aut>L. Ganman</aut>,
          <vrk><url ref="http://www.linusganman.se/g8u.htm">
          Matenson&gcirc;o</url></vrk>,
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_n;&c_t;&c_r;&c_a;&c_f;&c_a;&c_g;&c_o;&c_t;</trd>
  <trd lng="de">Kontrafagott</trd>
  <trdgrp lng="en">
    <trd>contrabassoon</trd>,
    <trd>double bassoon</trd>
  </trdgrp>
  <trd lng="fr">contrebasson</trd>
  <trd lng="pl">kontrafagot</trd>     
  <trd lng="ro">fagoturi duble</trd>
</drv>
</art>

