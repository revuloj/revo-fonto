<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: brosm.xml,v 1.5 2024/10/10 13:35:46 afido Exp $">
    <kap>
      <rad>brosm</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="brosm.0o">
  <kap><tld/>o</kap>
  <snc mrk="brosm.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" lst="voko:fi&scirc;oj" cel="fisx.mar0o">Marfi&scirc;o</ref> &gcirc;is unu metron longa, vivanta
      sur marfundo,
      trovebla en la norda Atlantika Maro,
      solspecia <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio
      <ref tip="malprt" cel="gaded.0oj">gadedoj</ref>
      (<trd lng="la">Brosme brosme</trd>):
      <ekz>
        li inter&scirc;an&gcirc;e ricevis sakon da sekala faruno, salitan <tld/>on, kelkajn funtojn da triktika
        faruno
        <fnt>
          <aut>H. Laxness, trad. B. Ragnarsson</aut>
          <vrk>Sendependaj homoj</vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/1/11/Brosme_brosme.jpg/799px-Brosme_brosme.jpg" prm="PD"><tld/>o
        <fnt>
          <lok><url ref="&WCW;/File:Brosme_brosme.jpg">&WCW;/File:Brosme_brosme.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Lumb</trd>
  </snc>
  <trd lng="fr">brosme</trd>
  <trd lng="pl">brosma</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_i;&#x43D;&#x44C;</trd>,
    <trd>&#x43C;&c_i;&#x43D;&#x44C;&#x43E;&#x43A;</trd>
  </trdgrp>
</drv>
</art>

