<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: magnet1.xml,v 1.19 2024/01/25 20:10:33 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>magnetism</rad>/o
</kap>
<drv mrk="magnet1.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="magnet1.0o.fenomeno">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Tuto de la <ref tip="super" cel="fenome.0o.FIL">fenomenoj</ref>
      koncernantaj <ref tip="super" cel="magnet.0o">magneto</ref>jn kaj
      movi&gcirc;antajn elektrajn <ref tip="super" cel="sxarg.0o.ELE">&scirc;argo</ref>jn:
      <ekz>
        la fero<tld/>o de kelkaj metaloj, precipe fero, sed anka&ubreve; nikelo kaj kobalto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=8229">Magnetismo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Maxwell</nom> formulis la ekvaciojn, kiuj priskribas la interefikon de elektra &scirc;argo
        kaj <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18559">Magneta kampo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la magneta dupoluso estas la plej elementa formo en kiu povas okazi <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=8229">Magnetismo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="magnet1.0o.fako">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Fako de la fiziko studanta tiujn fenomenojn.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_g;&c_n;&c_e;&c_t;&c_y;&c_z;&c_m;</trd>
  <trd lng="ca">magnetisme</trd>
  <trd lng="de">Magnetismus</trd>
  <trd lng="en">magnetism</trd>
  <trd lng="es">magnetismo</trd>
  <trd lng="fr">magn&eacute;tisme</trd>
  <trd lng="hu">m&aacute;gnesess&eacute;g</trd>
  <trdgrp lng="ja">
    <trd>&#x78C1;&#x6C17;  <pr>&#x3058;&#x304D;</pr></trd>,
    <trd>&#x78C1;&#x6027;  <pr>&#x3058;&#x305B;&#x3044;</pr></trd>,
    <trd>&#x78C1;&#x6C17;&#x5B66;  <pr>&#x3058;&#x304D;&#x304C;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">magnetisme</trd>
  <trdgrp lng="pl">
    <trd>magnetyzm</trd>,
    <trd>magnetyczno&sacute;&cacute;</trd>
  </trdgrp>
  <trd lng="pt">magnetismo</trd>
  <trd lng="ru">&c_m;&c_a;&c_g;&c_n;&c_je;&c_t;&c_i;&c_z;&c_m;</trd>
  <trd lng="uk">&#x43C;&#x430;&#x433;&#x43D;&#x435;&#x442;&#x438;&#x437;&#x43C;</trd>
  <trdgrp lng="zh">
    <trd>&#x78C1; <pr>c&#xED;</pr></trd>,
    <trd>&#x78C1;&#x6027; <pr>c&#xED;x&#xEC;ng</pr></trd>,
    <trd>&#x78C1;&#x529B; <pr>c&#xED;l&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>
