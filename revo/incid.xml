<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>incid</rad>/i
    </kap>
    <drv mrk="incid.0i">
      <kap><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <uzo tip="fak">FIZ</uzo>
      <uzo tip="fak">FOT</uzo>
      <snc>
        <dif>
          Fali sur surfacon:
          <ekz>
            kiam radio <tld/>as sur iun surfacon, tiu radio
            estas nomata <tld/>anta radio;
          </ekz><ekz>
            la <tld/>a ebeno estas ebeno, normala al la surfaco, kaj kiu
            entenas la <tld/>antan radion;
          </ekz><ekz>
            la <tld/>a angulo estas la angulo formata de la <tld/>anta
            radio kun la normalo &ccirc;e la <tld/>a punkto, t.e. &ccirc;e la
            punkto, kie la radio trafas la surfacon.
          </ekz>
        </dif>
        <trd lng="en">fall on (a surface)</trd>
        <trd lng="ru">&c_p;&c_a;&c_d;&c_a;&c_t;&c_mol; <klr>(&c_n;&c_a; &c_p;&c_o;&c_v;&c_je;&c_r;&c_h;&c_n;&c_o;&c_s;&c_t;&c_mol;, &c_f;&c_i;&c_z;.)</klr></trd>
      </snc>
    </drv>
    <drv mrk="incid.0a">
      <kap><tld/>a</kap>
      <snc mrk="incid.0a.grafee">
        <uzo tip="fak">MAT</uzo>
        <dif>
          En <ref tip="malprt" cel="grafe.0o">grafeo</ref>, vertico
          <frm>V</frm> kaj e&gcirc;o <frm>E</frm> estas incidaj, se
          <frm>V</frm> estas ekstremo de <frm>E</frm>:
          <ekz>
            du verticoj najbaras se ekzistas e&gcirc;o al kiu ili amba&ubreve; estas
            <tld/>aj.
          </ekz>
        </dif>
        <trd lng="en">incident</trd>
        <trd lng="ru">&c_i;&c_n;&c_c;&c_i;&c_d;&c_je;&c_n;&c_t;&c_n;&c_y;&c_j;</trd>
      </snc>
    </drv>
  </art>
<!--
$Log$
-->
</vortaro>
