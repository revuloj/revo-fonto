<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
   <art mrk="$Id$">
     <kap>
       <rad>vektor</rad>/o
     </kap>
     <drv mrk="vektor.0o">
       <kap><tld/>o</kap>
       <snc mrk="vektor.0o.MAT">
         <uzo tip="fak">MAT</uzo>
         <dif>
           <ref tip="super" cel="elemen.0o.MAT">Elemento</ref> de
           <ref tip="malprt" cel="modul.0o.magmo">modulo</ref> a&ubreve;
           <ref tip="malprt" cel="spac.vektora0o">vektora spaco</ref>.
         </dif>
       </snc>
       <snc mrk="vektor.0o.FIZ">
         <uzo tip="fak">FIZ</uzo>
         <dif>
           <tld lit="V"/>o <sncref ref="vektor.0o.MAT"/> en la kutima
           tri- a&ubreve; kvar-dimensia spaco, reprezentanta fizikan
           grandon, difinitan per sia intenseco kaj direkto:
           <ekz>rapido-<tld/>o;</ekz> <ekz>ondo-vektoro.</ekz>
         </dif>
       </snc>
       <trd lng="fr">vecteur</trd>
       <trd lng="ru">&c_v;&c_je;&c_k;&c_t;&c_o;&c_r;</trd>
     </drv>
   </art>

<!--
$Log$
-->
</vortaro>