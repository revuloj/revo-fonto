<vortaro>
<art mrk="$Id: aktuari.xml,v 1.2 2020/07/18 14:32:23 afido Exp $">
  <kap>
  <rad>aktuari</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="aktuari.0o">
  <kap><tld/>o</kap>
  <snc mrk="aktuari.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Asekura <ref tip="super" cel="matema.0isto.SCI">matematikisto</ref>
      kalkulanta riskojn, evoluojn, premiumojn:
      <ekz>
        <tld/>o &ccirc;e la vivasekura kompanio <nac>&leftquot;Scottish Equitable&rightquot;</nac>
        <fnt>
          <aut>Akademio de Esperanto</aut>,
          <vrk><url ref="http://www.akademio-de-esperanto.org/aktoj/aktoj1/historio.html">
          Aktoj de la Akademio 1963-1967</url></vrk>,
          <lok>[1967?]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">actuary</trd>
    <trd lng="fr">actuaire</trd>
  </snc>
</drv>
</art>

