<drv mrk="kroati.K0o">
  <kap>
    <tld lit="K"/>o
    <fnt><bib>RLN</bib></fnt>
  </kap>
  <snc mrk="kroati.K0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="kroat.K0ujo.GEOG">Kroatujo</ref>:
      <ekz>
        la Popola Armeo de eks-Jugoslavio enmar&scirc;is Bosnio-Hercegovinon post la retiro de siaj trupoj
        el Slovenio kaj <tld/>o
        <fnt>
          <aut>Hamza Baksi&cacute;</aut>
          <vrk><url ref="&Monato;/1993/A931001.php">La fino de la mondo komenci&gcirc;as en
          Sarajevo</url></vrk>
          <bib>Monato</bib>
          <lok>1993/10, p. 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        en <tld/>o, kiu havas 4,5 milionojn da lo&gcirc;antoj, vivis anta&ubreve; &ccirc;i tiu lasta milito proksimume
        15&nbsp;% da serboj, do proksimume 700&nbsp;000
        <fnt>
	<aut>Zlatko Ti&scaron;ljar</aut>
	<vrk><url ref="&Monato;/2000/006811.php">Nekredeblaj malveroj</url></vrk>
	<bib>Monato</bib>
	<lok>2000/03, p. 12-13</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_H;&c_a;&c_r;&c_v;&c_a;&c_t;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_H;&c_malmol;&c_r;&c_v;&c_a;&c_t;&c_i;&c_ja;</trd>
  <trd lng="de">Kroatien</trd>
  <trd lng="en">Croatia</trd>
  <trd lng="es">Croacia</trd>
  <trd lng="fr">Croatie</trd>
  <trd lng="id">Kroasia</trd>
  <trd lng="ja">&#x30AF;&#x30ED;&#x30A2;&#x30C1;&#x30A2;  <pr>&#x304F;&#x308D;&#x3042;&#x3061;&#x3042;</pr></trd>
  <trd lng="nl">Kroati&euml;</trd>
  <trdgrp lng="pl">
    <trd>Chorwacja</trd>,
    <trd>Republika <ind>Chorwacji</ind></trd>
