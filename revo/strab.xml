<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>strab</rad>/i
</kap>

<drv mrk="strab.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Nepovi, pro difekto de la okulaj muskoloj, direkti amba&ubreve; okulojn al
      sama celo.
    </dif>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_s;&c_a;&c_v;&c_u;&c_r;&c_y;&c_c;&c_c;&c_a;</trd>, 
    <trd>&c_k;&c_a;&c_s;&c_ib;&c_c;&c_c;&c_a;</trd>, 
    <trd>&c_g;&c_l;&c_ja;&c_d;&c_z;&c_je;&c_c;&c_mol;
&c_s;&c_k;&c_o;&c_s;&c_a;</trd>, 
    <trd>&c_b;&c_y;&c_c;&c_mol; &c_k;&c_a;&c_s;&c_a;&c_v;&c_o;&c_k;&c_ib;&c_m;</trd>
  </trdgrp>
  <trd lng="de">schielen</trd>
  <trd lng="en">squint</trd>
  <trd lng="fr">loucher</trd>
  <trdgrp lng="hu">
    <trd>kancsal&iacute;t</trd>,
    <trd>bandzs&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">scheel kijken</trd>
  <trdgrp lng="pt">
    <trd>vesguear</trd>,
    <trd>ser <ind>vesgo</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_s;&c_i;&c_t;&c_mol; <klr
tip="amb">(&c_g;&c_l;&c_a;&c_z;&c_a;&c_m;&c_i;)</klr></trd>,
    <trd>&c_k;&c_o;&c_s;&c_o;&c_g;&c_l;&c_a;&c_z;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>ser estr&aacute;bico, -a</trd>,
    <trd>ser bizco, -a</trd>
  </trdgrp>
  </snc>

  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Rigardi de flanke, kun malica a&ubreve; envia intenco:
      <ekz>&scirc;iaj pupiloj <tld/>is al la lito.</ekz>
    </dif>
    <trd lng="hu">ferd&eacute;n n&eacute;z</trd>
    <trd lng="pt">olhar de <ind>soslaio</ind></trd>
  </snc>
</drv>

<drv mrk="strab.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      Stato, en kiu amba&ubreve; okuloj de persono ne povas samtempe
      rigardi unu saman punkton (ofte nur unu el la okuloj estas tute sana):
      <ekz><ind>diver&gcirc;a <tld/>ismo</ind>;</ekz>
      <trdgrp lng="hu">
        <trd>strabismus divergens</trd>,
        <trd>sz&eacute;tt&eacute;r&odblac; kancsals&aacute;g</trd>
      </trdgrp>
      <ekz><ind>konver&gcirc;a <tld/>ismo</ind>;</ekz>
      <trdgrp lng="hu">
        <trd>strabismus convergens</trd>,
        <trd>&ouml;sszet&eacute;r&odblac; <ind>kancsls&aacute;g</ind></trd>
      </trdgrp>
      <ekz>paraliza <tld/>ismo</ekz>
      <trdgrp lng="hu">
        <trd>strabismus paralyticus</trd>,
        <trd>b&eacute;nul&aacute;sos <ind>kancsals&aacute;g</ind></trd>
      </trdgrp>
      <ekz>konstanta <tld/>ismo</ekz>      
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Schielen</trd>,
    <trd>Strabismus</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>strabismus</trd>,
    <trd>kancsals&aacute;g</trd>
  </trdgrp>
  <trd lng="es">estrabismo</trd>
  <trd lng="pt">estrabismo</trd>
</drv>

<drv mrk="strab.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Trafita de <tld/>ismo:
      <ekz><tld/>a infano;</ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi a&ubreve;dis neniun ripro&ccirc;an vorton, vidis neniun <tld/>an
        <klr>(= malican)</klr> rigardon<fnt>K</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">schielend</trd>
  <trd lng="es">estr&aacute;bico, -a</trd>
  <trdgrp lng="hu">
    <trd>kancsal</trd>,
    <trd>bandzsa</trd>
  </trdgrp>
</drv>

<drv mrk="strab.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      En <tld/>a maniero:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>e rigardi la ri&ccirc;a&jcirc;ojn de aliaj;
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_s;&c_a;&c_v;&c_u;&c_r;&c_a;</trd>, 
    <trd>&c_k;&c_a;&c_s;&c_a;&c_v;&c_o;&c_k;&c_a;</trd> 
  </trdgrp>
  <trd lng="de">schielend</trd>
  <trd lng="hu">ferd&eacute;n</trd>
  <trdgrp lng="es">
    <trd>estr&aacute;bicamente</trd>,
    <trd>de reojo<klr> (con malicia)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>estrabicamente</trd>,
    <trd>de <ind>soslaio</ind></trd>
  </trdgrp>
</drv>

<drv mrk="strab.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Persono <tld/>a:
      <ekz>
        <ind>inter blinduloj re&gcirc;as <tld/>uloj</ind><fnt>Z</fnt>;
        <trd lng="hu">vakok k&ouml;z&ouml;tt f&eacute;lszem&udblac; a kir&aacute;ly</trd>
        <trd lng="es">En el pa&iacute;s de los ciegos, el tuerto es el Rey</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">schielender (Mensch)</trd>
  <trdgrp lng="es">
   <trd>bosojo, -a</trd>,
   <trd>virolo, -a</trd>,
   <trd>bizco, -a</trd>
  </trdgrp>
  <trd lng="hu">kancsal ember</trd>  
  <trdgrp lng="pt">
    <trd>vesgo</trd>,
    <trd>estr&#225;bico</trd>
  </trdgrp>
</drv>



</art>
