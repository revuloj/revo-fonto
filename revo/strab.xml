<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>strab</rad>/i
</kap>
<drv mrk="strab.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Nepovi, pro difekto de la okulaj muskoloj, direkti amba&ubreve; okulojn al
      sama celo:
      <ekz>
        inter blinduloj re&gcirc;as <tld/>uloj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind><tld/>e</ind> rigardi la ri&ccirc;a&jcirc;ojn de aliaj;
        <trdgrp lng="be">
          <trd>&c_k;&c_a;&c_s;&c_a;&c_v;&c_u;&c_r;&c_a;</trd>, 
          <trd>&c_k;&c_a;&c_s;&c_a;&c_v;&c_o;&c_k;&c_a;</trd> 
        </trdgrp>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi a&ubreve;dis neniun ripro&ccirc;an vorton, vidis neniun <tld/>an
        <klr>(= malican)</klr> rigardon<fnt>K</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_s;&c_a;&c_v;&c_u;&c_r;&c_y;&c_c;&c_c;&c_a;</trd>, 
    <trd>&c_k;&c_a;&c_s;&c_ib;&c_c;&c_c;&c_a;</trd>, 
    <trd>&c_g;&c_l;&c_ja;&c_d;&c_z;&c_je;&c_c;&c_mol; &c_s;&c_k;&c_o;&c_s;&c_a;</trd>, 
    <trd>&c_b;&c_y;&c_c;&c_mol; &c_k;&c_a;&c_s;&c_a;&c_v;&c_o;&c_k;&c_ib;&c_m;</trd>
  </trdgrp>
  <trd lng="de">schielen</trd>
  <trd lng="en">squint</trd>
  <trd lng="nl">scheel kijken</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_s;&c_i;&c_t;&c_mol; <klr tip="amb">(&c_g;&c_l;&c_a;&c_z;&c_a;&c_m;&c_i;)</klr></trd>,
    <trd>&c_k;&c_o;&c_s;&c_o;&c_g;&c_l;&c_a;&c_z;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
