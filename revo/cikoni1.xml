<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cikoni1.xml,v 1.10 2023/10/31 13:37:03 revo Exp $">
<kap>
  <rad>cikonied</rad>/oj
</kap>
<drv mrk="cikoni1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_familioj" cel="famili.0o.BIO">Familio</ref>
      el la ordo <ref tip="super" cel="bird.vad0oj">vadbirdoj</ref>
      (<trd lng="la">Ciconiidae</trd>):
      <ekz>
        <tld/>oj <klr>[&#x2026;]</klr> havas 6 genrojn kaj 19 speciojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17327">Cikoniedoj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la Abdima <ref tip="prt" cel="cikoni.0o">cikonio</ref>,
        kiu nomi&gcirc;as anka&ubreve; Blankbrusta cikonio <klr>[&#x2026;]</klr> estas
        specio de la genro <klr>[&#x2026;]</klr> cikonioj, nome tipa genro en la familio
        <tld lit="C"/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=254544">Abdima cikonio</url></lok>
        </fnt>.
      </ekz>      
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_s;&c_l;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">St&ouml;rche <klr tip="ind">(Familie)</klr></trd>
  <trd lng="fr">ciconiid&eacute;s</trd>
  <trd lng="he">&#x5D7;&#x5E1;&#x5D9;&#x5D3;&#x5D5;&#x5EA;</trd>
  <trd lng="hu">g&oacute;lyaf&eacute;l&eacute;k</trd>
  <trd lng="pl">bocianowate</trd>
  <trd lng="pt">cicon&iacute;deos</trd>
  <trd lng="ru">&c_a;&c_i;&c_s;&c_t;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="zh">&#x9E1B;&#x79D1; <pr>gu&#xE0;nk&#x113;</pr></trd>
</drv>
</art>

