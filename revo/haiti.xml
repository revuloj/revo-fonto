<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>haiti</rad>/o
</kap>

<drv mrk="haiti.0o">
  <kap>
    <tld lit="H"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="haiti.0o.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      &Scirc;tato en la Kariba Maro okupanta okcidentan parton de insulo
      Hispaniolo
      kaj aron da &ccirc;irka&ubreve;aj insuloj, kun &ccirc;efurbo
      Portoprinco.
      ISO-3166 kodo: HT.
      &Gcirc;i limas kun
      <ref tip="vid" cel="dominik2.0o.sxtato">Dominika Respubliko</ref>.
    </dif>
    <trd lng="bg">&c_H;&c_a;&c_i;&c_t;&c_i;</trd>
    <trd lng="de">Haiti</trd>
    <trd lng="en">Haiti</trd>
    <trd lng="it">Haiti</trd>
    <trdgrp lng="pl">
      <trd>Haiti</trd>,
      <trd>Republika <ind>Haiti</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

