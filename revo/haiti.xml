<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: haiti.xml,v 1.21 2024/10/05 12:47:40 revo Exp $">
<kap><ofc>10</ofc>
  <rad>Haiti</rad>/o
  <fnt><bib>RLN</bib></fnt>
</kap>

<drv mrk="haiti.0o">
  <kap>
    <ofc>10</ofc><tld/>o
  </kap>
  <mlg>HT</mlg>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
  <snc mrk="haiti.0o.sxtato">
    <dif>
      <ref tip="lst" cel="sxtat.0o" lst="voko:&scirc;tatoj">&Scirc;tato</ref> en la
      <ref tip="malprt" cel="mar.0o">Kariba Maro</ref>
      okupanta la okcidentan parton de la insulo Hispaniolo kaj aron da
      &ccirc;irka&ubreve;aj insuloj, kun &ccirc;efurbo Portoprinco;
      &gcirc;i limas kun
      <ref tip="vid" cel="respub.Dominika_0o.POL">Dominika Respubliko</ref>:
      <ekz>
        unu el la multaj, kiuj perdis familianojn kaj sian tutan hava&jcirc;on en <tld/>o
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Monato;/2010/009994.php">Mi vivas surstrate ...</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la novan &scirc;taton <klr>[Biafron]</klr> agnoskis nur kvar afrikaj landoj kaj <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2012/010625.php">Batalanto universitata</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_G;&c_a;&c_ib;&c_c;&c_ib;</trd>
    <trd lng="bg">&c_H;&c_a;&c_i;&c_t;&c_i;</trd>
    <trdgrp lng="ca">
      <trd>Hait&iacute;</trd>,
      <trd>Rep&uacute;blica d'Hait&iacute;</trd>
    </trdgrp>
    <trd lng="de">Haiti</trd>
    <trd lng="en">Haiti</trd>
    <trd lng="es">Hait&iacute;</trd>
    <trd lng="fr">Ha&iuml;ti</trd>
    <trd lng="nl">Ha&iuml;ti</trd>
    <trd lng="id">Haiti</trd>
    <trd lng="it">Haiti</trd>
    <trdgrp lng="pl">
      <trd>Haiti</trd>,
      <trd>Republika <ind>Haiti</ind></trd>
    </trdgrp>
    <trd lng="pt">Haiti</trd>
    <trd lng="zh">&#28023;&#22320; <pr>h&#462;id&igrave;</pr></trd>
  </snc>
  <trd lng="ja">&#12495;&#12452;&#12481;  <pr>&#12399;&#12356;&#12385;</pr></trd>
  <trd lng="uk">&c_G;&c_a;&c_ji;&c_t;&c_ib;</trd>
</drv>

<drv mrk="haiti.0a">
  <kap>
    <ofc>10</ofc><tld lit="h"/>a
  </kap>
  <snc>
    <dif>
      Rilata al <tld/>o, la <tld lit="h"/>anoj ktp.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>haitisch</trd>,
    <trd>haitianisch</trd>
  </trdgrp>
</drv>

<drv mrk="haiti.0ano">
  <kap>
    <ofc>10</ofc><tld lit="h"/>ano
  </kap>
  <snc>
    <dif>
      Iu el <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Haitianer</trd>
</drv>

</art>

