<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rezidu.xml,v 1.6 2022/07/15 07:30:30 revo Exp $">
<kap>
  <rad>rezidu</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="rezidu.0o">
  <kap><tld/>o</kap>
  <snc mrk="rezidu.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Resta&jcirc;o, rubo, neutila produkto de reakcio:
      <ekz>
        la afero de la radiaktivaj <klr>...</klr> <tld/>oj estas 
        serioza kialo kontra&ubreve; atomenergiaj elektrocentraloj
        <fnt>
          <aut>L. G. Restrepo Rivas</aut>,
          <vrk><url ref="http://www.delbarrio.eu/blo/2011/03/timo-sento/#comment-468">
          Timo kaj aliaj sentoj</url></vrk>,
          <lok>2011-03-24</lok>
        </fnt>.
      </ekz>
    </dif>
    <rim mrk="rezidu.0o.RIM_SIN">
      Alternative oni povas vastigi la sencon de la pli malnova
      <ref tip="vid" cel="skori.0o.TEK">skorio</ref>.
      <aut>&jcirc;v</aut>
    </rim>
    <refgrp tip="vid">
      <ref cel="fecx.0o.AGR">fe&ccirc;o</ref>,
      <ref cel="rekrem.0o.AGR">rekremento</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_a;&c_s;&c_a;&c_d;&c_a;&c_k;</trd>,
      <trd>&c_a;&c_p;&c_a;&c_d;&c_a;&c_k;</trd>,
      <trd>&c_r;&c_e;&c_sh;&c_t;&c_k;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>bottoms</trd>,
      <trd>residue</trd>
    </trdgrp>
    <trd lng="fr">r&eacute;sidu<klr> (chimie)</klr></trd>
    <trd lng="id">residu</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>reziduum</trd>,
    <trd>zbytek</trd>,
    <trd>z&#x16F;statek</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rez&#xED;duum</trd>,
    <trd>zostatok</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&#x43B;&c_i;&c_sh;&#x43E;&#x43A;</trd>,
    <trd>&#x43E;&c_s;&c_a;&c_d;</trd>,
    <trd>&#x43E;&c_s;&c_a;&c_d;&#x43E;&#x43A;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_s;&c_t;&c_ib;&c_j;</trd>,
    <trd>&c_sh;&#x43B;&c_a;&#x43C;</trd>,
    <trd>&c_z;&c_a;&#x43B;&c_i;&c_sh;&#x43E;&#x43A;</trd>,
    <trd>&c_r;&c_je;&c_sh;&c_t;&c_a;</trd>
  </trdgrp>
</drv>
</art>

