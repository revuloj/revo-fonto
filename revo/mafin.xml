<drv mrk="mafin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Malgranda ronda <ref tip="super" cel="bak.0ajxo.KUI">baka&jcirc;o</ref>,
      bakita en speciala formo kun kavetoj: rost<ref tip="super" cel="bulk.0o">bulko</ref> en
      Britujo,
      <ref tip="super" cel="kuk.0eto">kuketo</ref> en Usono:
      <ekz>
        veganaj <tld/>oj kun &ccirc;okolad-gutoj
        <fnt>
          <aut>Andy</aut>
          <vrk><url ref="https://veganarto.com/veganaj-mafinoj-kun-cokolad-gutoj/">Veganaj mafinoj
          kun &ccirc;okolad-gutoj</url></vrk>
          <lok>VeganArto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la brita <tld/>o estas <klr>[&#8230;]</klr> pretigita per gistofungo kaj estas man&gcirc;ata kiel
        rostpano
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=386669">Mafino</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Muffin</trd>,
      <trd>Toastbr&ouml;tchen</trd>
    </trdgrp>
    <trd lng="en">Muffin</trd>
    <trd lng="fr">Muffin</trd>
