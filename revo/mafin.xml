  <trd lng="be">&c_m;&c_a;&c_f;&c_ib;&c_n;</trd>
  <trdgrp lng="zh">
    <trd>&#22937;&#33433; <pr>mi&#xE0;of&#xFA;</pr></trd>,
    <trd>&#x5C0F;&#x677E;&#x7CD5; <pr>xi&#x1CE;os&#x14D;ngg&amacron;o</pr></trd>,
    <trd>&#x5C0F;&#x9B06;&#x7CD5; <pr>xi&#x1CE;os&#x14D;ngg&amacron;o</pr></trd>
  </trdgrp>
</drv>

