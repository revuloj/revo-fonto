  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#20605;&#29289;  <pr>&#12395;&#12379;&#12418;&#12398;</pr></trd>,
    <trd>&#36107;&#20316;  <pr>&#12364;&#12435;&#12373;&#12367;</pr></trd>,
    <trd>&#27169;&#36896;&#21697;  <pr>&#12418;&#12382;&#12358;&#12402;&#12435;</pr></trd>,
    <trd>&#20316;&#12426;&#29289;  <pr>&#12388;&#12367;&#12426;&#12418;&#12398;</pr></trd>
  </trdgrp>
  <trd lng="pl">fa&lstroke;sz</trd>
  <trd lng="ro">fals</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_ib;&c_d;&c_r;&c_o;&c_b;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_r;&c_o;&c_b;&c_l;&c_je;&c_n;&c_a; &c_r;&c_ib;&c_ch;</trd>,
    <trd>&c_f;&c_a;&c_l;&c_mol;&c_s;&c_i;&c_f;&c_ib;&c_k;&c_a;&c_t;</trd>,
    <trd>&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_i;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20551;&#36008; <pr>ji&#462;hu&ograve;</pr></trd>,
    <trd>&#20223;&#20882;&#21697; <pr>f&#462;ngm&agrave;op&#464;n</pr></trd>,
    <trd>&#20266;&#36896; <pr>w&ecaron;iz&agrave;o</pr></trd>,
    <trd>&#20551;&#20882; <pr>ji&#462;m&agrave;o</pr></trd>,
    <trd>&#20266;&#38046; <pr>w&ecaron;ich&amacron;o</pr></trd>,
    <trd>&#20266;&#36896;&#21697; <pr>w&ecaron;iz&agrave;op&#464;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fals.0ema">
  <kap><tld/>ema, <var><kap><tld/>ama</kap></var></kap>
  <snc mrk="fals.0ema.malsincera">
    <dif>
      Malsincera, mensoga:
      <ekz>
        kiu ka&scirc;as malamon, tiu havas <tld/>eman bu&scirc;on
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 10:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        atestanto <tld/>ama elspiras mensogojn
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 14:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>emo pereigos la maliculojn
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 11:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne sidas kun homoj malveremaj, kaj kun <tld/>emuloj mi ne
        iros
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 26:4</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hipokr.0a">hipokrita</ref>,
      <ref cel="jezuit">jezuita</ref>
    </refgrp>
