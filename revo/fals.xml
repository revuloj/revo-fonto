<drv mrk="fals.0a">
  <kap><tld/>a</kap>
  <snc mrk="fals.0a.falsema">
    <dif>
      <tld lit="F"/>anta:
      <ekz>
        <tld/>a atestanto, kiu elspiras mensogojn
        <fnt><bib>MT</bib>, <lok>&Sen; 6:19</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a lango
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 120:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj profetoj, kiuj venas al vi en &scirc;afaj feloj, sed interne
        estas rabemaj
        lupoj
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 7:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj instruistoj, kiuj sekrete enkondukos herezojn pereigajn
        <fnt>
          <bib>NT</bib>
          <lok>II. Petro 2:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">LIN</uzo>
        <ref cel="amik.falsa0o">falsa amiko</ref>
        <fnt>
          <bib>Monato</bib>
          <aut>Petro De Smet'</aut>
          <vrk><url ref="&Monato;/2008/009292.php">Sidante
          en pudingo, mi alvokas miajn savantojn!</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_ib;&c_l;&c_zh;&c_y;&c_v;&c_y;</trd>
    <trd lng="en">false</trd>
    <trdgrp lng="pl">
      <trd>zak&lstroke;amany</trd>,
      <trd>pomawiaj&aogonek;cy</trd>,
      <trd>obmawiaj&aogonek;cy</trd>,
      <trd>krzywoprzysi&eogonek;&zdot;ny</trd>
    </trdgrp>
    <trd lng="ru">&c_l;&c_zh;&c_i;&c_v;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="fals.0a.malauxtenta">
    <dif>
      Malvera, nea&ubreve;tenta:
      <ekz>
        <tld/>a aserto, kalkulo, pentra&jcirc;o, subskribo;
      </ekz>
      <ekz>
        <tld/>a nova&jcirc;o
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Monato;/2004/008268.html">Poemoj
          de Morteza Mirbaghian</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a sekureco
        <fnt>
          <bib>Monato</bib>
          <aut>Peggy Ley</aut>
          <vrk><url ref="&Monato;/2008/009553.php">Granda
          frato gvatas (2)</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a nomo por ka&scirc;i la identecon de la familio
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="&Monato;/2009/009911.php">Rifu&gcirc;intoj pro
          domperdo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;efministro estas <tld/>a in&gcirc;eniero
        <fnt>
          <bib>Monato</bib>
          <aut>Jo&atilde;o Jos&eacute; Santos</aut>
          <vrk><url ref="&Monato;/2007/009262.php">Diantoj
          ... kaj dornoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="auxten.0a">a&ubreve;tentika</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_y;&c_v;&c_y;</trd>,
      <trd>&c_n;&c_je;&c_s;&c_a;&c_p;&c_r;&c_a;&c_w;&c_d;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>false</trd>,
      <trd>counterfeit</trd>,
      <trd>fake</trd>,
      <trd>forged</trd>,
      <trd>spurious</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>fa&lstroke;szywy</trd>,
      <trd>sfa&lstroke;szowany</trd>,
      <trd>podrobiony</trd>,
      <trd>spreparowany<klr> (np. dow&oacute;d)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_d;&c_d;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_l;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_i;&c_v;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="fals.0a.falsita">
    <dif>
      <tld lit="F"/>ita:
      <ekz>
        <tld/>a pesilo
        <fnt>
          <bib>MT</bib>
          <lok>Ho&scirc;ea 12:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        treege granda <tld/>a buklo pendis super unu el &scirc;iaj okuloj
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        estis na&ubreve; okazoj de falsa sorgobrando
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="&Monato;/2004/008368.html">Maltrankvili&gcirc;o
          pro a&ccirc;aj man&gcirc;a&jcirc;oj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a mono ekestis en la presejo, kiun la polico
        <klr>[&#x2026;]</klr> sukcese
        senvualigis
        <fnt>
          <bib>Monato</bib>
          <aut>Last</aut>
          <vrk><url ref="&Monato;/2004/008497.html">Falsaj
          e&ubreve;roj dama&gcirc;as ekonomion</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="natur">nenatura</ref>,
      <ref cel="art">arta&jcirc;a</ref>.
    </refgrp>
    <trd lng="be">&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_y;&c_v;&c_y;</trd>
    <trdgrp lng="en">
      <trd>false</trd>,
      <trd>counterfeit</trd>,
      <trd>fake</trd>,
      <trd>forged</trd>,
      <trd>spurious</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>fa&lstroke;szywy</trd>,
      <trd>sztuczny</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_d;&c_d;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_l;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_i;&c_v;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="fals.0a.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Ne&gcirc;usta, erara:
      <ekz>
        <tld/>a noto;
      </ekz>
      <ekz>
        pioniro kun ru&gcirc;a koltuko ekstaris &ccirc;e la lernejo-pordo kaj tre
        <tld/>e ekkantis
        ian pioniran kanton himnecan
        <fnt>
          <bib>Monato</bib>
          <aut>Jozefo Horv&aacute;th</aut>
          <vrk><url ref="&Monato;/2006/009060.php">Tra
          infanaj okuloj: memoroj pri la eventoj de '56</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_y;&c_v;&c_y;</trd>
    <trd lng="en">wrong</trd>
    <trd lng="pl">fa&lstroke;szuj&aogonek;cy</trd>
    <trd lng="ru">&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_i;&c_v;&c_y;&c_j;</trd>
  </snc>
  <trd lng="bo">&t_ra;&t_dzata;&t_shabchu;&t_na;&t_tsheg;&t_ma;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>fale&scaron;n&yacute;</trd>,
    <trd>k&rcaron;iv&yacute;</trd>,
    <trd>lich&yacute;</trd>,
    <trd>l&zcaron;iv&yacute;</trd>,
    <trd>nepravdiv&yacute;</trd>,
    <trd>neprav&yacute;</trd>,
    <trd>nep&uring;vodn&iacute;</trd>,
    <trd>pad&ecaron;lan&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>falsch</trd>,
    <trd>gef&auml;lscht</trd>,
    <trd>unecht</trd>
  </trdgrp>
  <trd lng="es">falso</trd>
  <trdgrp lng="fr">
    <trd>faux<klr tip="ind"> (adj.)</klr></trd>,
    <trd>mensonger</trd>
  </trdgrp>
  <trd lng="hu">hamis</trd>
  <trd lng="ja">&#x507D;&#x306E;  <pr>&#x304E;&#x306E;</pr></trd>
  <trd lng="nl">vals</trd>
  <trdgrp lng="sk">
    <trd>falo&scaron;n&yacute;</trd>,
    <trd>ne&uacute;primn&yacute;</trd>
  </trdgrp>
  <trd lng="zh">&#x4F2A; <pr>w&#x11B;i</pr></trd>
</drv>

<drv mrk="fals.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="fals.0ajxo.KOMUNE">
    <dif>
      Rezulto de <tld/>ado: mala&ubreve;tenta objekto,
      malvera informo kaj similaj:
      <ekz>
        via lango plektas <tld/>a&jcirc;on
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 50:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        misgvidi homojn cele ke ili kredu <tld/>a&jcirc;on
        <fnt>
          <aut>S. Abbasi, trad Trigo Peng Zhengming</aut>
          <vrk>Leksikono por la cifereca epoko</vrk>
          <lok><url ref="&UEA;pdf/Unesko-Kuriero_2-2017.pdf">
          Unesko-kuriero, 2017-07 &gcirc;is 09 (2)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_r;&c_o;&c_b;&c_k;&c_a;</trd>,
    <trd>&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_y;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_f;&c_a;&c_l;&c_mol;&c_s;&c_y;&c_f;&c_ib;&c_k;&c_a;&c_t;</trd>
  </trdgrp>
  <trd lng="de">F&auml;lschung</trd>
  <trdgrp lng="fr">
    <trd>falsification</trd>
    <trd>faux<klr> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x507D;&#x7269;  <pr>&#x306B;&#x305B;&#x3082;&#x306E;</pr></trd>,
    <trd>&#x8D0B;&#x4F5C;  <pr>&#x304C;&#x3093;&#x3055;&#x304F;</pr></trd>,
    <trd>&#x6A21;&#x9020;&#x54C1;  <pr>&#x3082;&#x305E;&#x3046;&#x3072;&#x3093;</pr></trd>,
    <trd>&#x4F5C;&#x308A;&#x7269;  <pr>&#x3064;&#x304F;&#x308A;&#x3082;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_ib;&c_d;&c_r;&#x43E;&c_b;&#x43A;&c_a;</trd>,
    <trd>&#x43F;&c_ib;&c_d;&c_r;&#x43E;&c_b;&#x43B;&c_je;&#x43D;&c_a; &c_r;&c_ib;&c_ch;</trd>,
    <trd>&c_f;&c_a;&#x43B;&#x44C;&c_s;&c_i;&c_f;&c_ib;&#x43A;&c_a;&c_t;</trd>,
    <trd>&c_f;&c_a;&#x43B;&#x44C;&c_sh;&c_i;&c_v;&#x43A;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5047;&#x8CA8; <pr>ji&#x1CE;hu&#xF2;</pr></trd>,
    <trd>&#x4EFF;&#x5192;&#x54C1; <pr>f&#x1CE;ngm&#xE0;op&#x1D0;n</pr></trd>,
    <trd>&#x4F2A;&#x9020; <pr>w&#x11B;iz&#xE0;o</pr></trd>,
    <trd>&#x5047;&#x5192; <pr>ji&#x1CE;m&#xE0;o</pr></trd>,
    <trd>&#x4F2A;&#x949E; <pr>w&#x11B;ich&#x101;o</pr></trd>,
    <trd>&#x4F2A;&#x9020;&#x54C1; <pr>w&#x11B;iz&#xE0;op&#x1D0;n</pr></trd>
