    <trd>falsifier<klr tip="ind"> (contrefaire)</klr></trd>,
    <trd>frelater</trd>
  </trdgrp>
  <trd lng="hu">hamis&iacute;t</trd>
  <trdgrp lng="id">
    <trd><baz>lancung</baz>melancungkan</trd>,
    <trd><baz>palsu</baz>memalsukan</trd>,
    <trd><baz>tipu</baz>menipu</trd>,
    <trd><baz>tiru</baz>meniru</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#20605;&#36896;&#12377;&#12427;  <pr>&#12366;&#12382;&#12358;&#12377;&#12427;</pr></trd>,
    <trd>&#22793;&#36896;&#12377;&#12427;  <pr>&#12408;&#12435;&#12382;&#12358;&#12377;&#12427;</pr></trd>,
    <trd>&#20605;&#20316;&#12377;&#12427;  <pr>&#12366;&#12388;&#12367;&#12377;&#12427;</pr></trd>,
    <trd>&#25913;&#31428;&#12377;&#12427;  <pr>&#12363;&#12356;&#12374;&#12435;&#12377;&#12427;</pr></trd>,
    <trd>&#20605;&#12427;  <pr>&#12356;&#12388;&#12431;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">vervalsen</trd>
  <trdgrp lng="pl">
    <trd>fa&lstroke;szowa&cacute;</trd>,
    <trd>podrabia&cacute;</trd>,
    <trd>falsyfikowa&cacute;</trd>,
    <trd>preparowa&cacute;<klr> (np. dowody)</klr></trd>
