<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>fals</rad>/i
</kap>
<drv mrk="fals.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fals.0i.KOMUNE">
    <dif>
      Trompe &scirc;an&gcirc;i, aliigi la staton a&ubreve; konsiston de
      io;
      trompe imiti la &scirc;ajnon de io:
      <ekz>
        <tld/>ita lakto, vino;
      </ekz>
      <ekz>
        <tld/>i dokumenton<fnt>B</fnt>, la veron;
      </ekz>
      <ekz>
        oni poste <tld/>is la sencon de miaj vortoj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_r;&c_a;&c_b;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>falzifikovat</trd>,
    <trd>fal&scaron;ovat</trd>,
    <trd>pad&ecaron;lat</trd>,
    <trd>podvrhnout</trd>
  </trdgrp>
  <trd lng="de">f&auml;lschen</trd>
  <trdgrp lng="en">
    <trd>falsify</trd>,
    <trd>counterfeit</trd>,
    <trd>fake</trd>,
    <trd>forge</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>falsear</trd>,
    <trd>falsificar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>contrefaire</trd>,
    <trd>falsifier<klr tip="ind"> (contrefaire)</klr></trd>,
    <trd>frelater</trd>
  </trdgrp>
  <trd lng="hu">hamis&iacute;t</trd>
  <trd lng="nl">vervalsen</trd>
  <trdgrp lng="pl">
    <trd>fa&lstroke;szowa&cacute;</trd>,
    <trd>podrabia&cacute;</trd>,
    <trd>falsyfikowa&cacute;</trd>,
    <trd>preparowa&cacute;<klr> (np. dowody)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>falsificar</trd>,
    <trd>falsear</trd>,
    <trd>adulterar</trd>,
    <trd>deturpar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_d;&c_je;&c_l;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_f;&c_a;&c_l;&c_mol;&c_s;&c_i;&c_f;&c_i;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">fal&scaron;ova&tcaron;</trd>
</drv>

<drv mrk="fals.0a">
  <kap><tld/>a</kap>
  <snc mrk="fals.0a.falsema">
    <dif>
      <tld lit="F"/>anta:
      <ekz>
        <tld/>a atestanto, kiu elspiras mensogojn
        <fnt><bib>MT</bib>, <lok>&Sen; 6:19</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a lango<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_ib;&c_l;&c_zh;&c_y;&c_v;&c_y;</trd>
    <trd lng="en">false</trd>
    <trdgrp lng="pl">
      <trd>zak&lstroke;amany</trd>,
      <trd>pomawiaj&aogonek;cy</trd>,
      <trd>obmawiaj&aogonek;cy</trd>,
      <trd>krzywoprzysi&eogonek;&zdot;ny</trd>
    </trdgrp>
    <trd lng="ru">&c_l;&c_zh;&c_i;&c_v;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="fals.0a.malauxtenta">
    <dif>
      Malvera, nea&ubreve;tenta:
      <ekz>
        <tld/>a aserto, kalkulo, pentra&jcirc;o, subskribo.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="auxten1.0a">a&ubreve;tentika</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_y;&c_v;&c_y;</trd>,
      <trd>&c_n;&c_je;&c_s;&c_a;&c_p;&c_r;&c_a;&c_w;&c_d;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>false</trd>,
      <trd>counterfeit</trd>,
      <trd>fake</trd>,
      <trd>forged</trd>,
      <trd>spurious</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>fa&lstroke;szywy</trd>,
      <trd>sfa&lstroke;szowany</trd>,
      <trd>podrobiony</trd>,
      <trd>spreparowany<klr> (np. dow&oacute;d)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_d;&c_d;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_l;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_i;&c_v;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="fals.0a.falsita">
    <dif>
      <tld lit="F"/>ita:
      <ekz>
        <tld/>a dento, hararo.
      </ekz>
    </dif>
    <adm>
      Pli bona ekzemplo estus "~a mono". Ja ~aj dentoj aux
      hararo ne estas ~itaj! Cxu tamen oni diras "~a vino"?
      <aut>MB</aut>
    </adm>
    <refgrp tip="vid">
      <ref cel="natur">nenatura</ref>,
      <ref cel="art">arta&jcirc;a</ref>.
    </refgrp>
    <trd lng="be">&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_y;&c_v;&c_y;</trd>
    <trdgrp lng="en">
      <trd>false</trd>,
      <trd>counterfeit</trd>,
      <trd>fake</trd>,
      <trd>forged</trd>,
      <trd>spurious</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>fa&lstroke;szywy</trd>,
      <trd>sztuczny</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_d;&c_d;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_l;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_i;&c_v;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="fals.0a.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Ne&gcirc;usta:
      <ekz>
        <tld/>a noto;
      </ekz>
      <ekz>
        <tld/>e kanti.
      </ekz>
    </dif>
    <trd lng="be">&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_y;&c_v;&c_y;</trd>
    <trd lng="en">wrong</trd>
    <trd lng="pl">fa&lstroke;szuj&aogonek;cy</trd>
    <trd lng="ru">&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_i;&c_v;&c_y;&c_j;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>fale&scaron;n&yacute;</trd>,
    <trd>k&rcaron;iv&yacute;</trd>,
    <trd>lich&yacute;</trd>,
    <trd>l&zcaron;iv&yacute;</trd>,
    <trd>nepravdiv&yacute;</trd>,
    <trd>neprav&yacute;</trd>,
    <trd>nep&uring;vodn&iacute;</trd>,
    <trd>pad&ecaron;lan&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>falsch</trd>,
    <trd>gef&auml;lscht</trd>,
    <trd>unecht</trd>
  </trdgrp>
  <trd lng="es">falso</trd>
  <trdgrp lng="fr">
    <trd>faux<klr tip="ind"> (adj.)</klr></trd>,
    <trd>mensonger</trd>
  </trdgrp>
  <trd lng="hu">hamis</trd>
  <trd lng="nl">vals</trd>
  <trdgrp lng="sk">
    <trd>falo&scaron;n&yacute;</trd>,
    <trd>ne&uacute;primn&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="fals.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="fals.0ajxo.KOMUNE">
    <dif>
      Rezulto de <tld/>ado: mala&ubreve;tenta objekto,
      malvera informo kaj similaj:
      <ekz>
        misgvidi homojn cele ke ili kredu <tld/>a&jcirc;on
        <fnt>
          <aut>S. Abbasi, trad Trigo Peng Zhengming</aut>
          <vrk>Leksikono por la cifereca epoko</vrk>
          <lok><url ref="http://uea.org/pdf/Unesko-Kuriero_2-2017.pdf">
          Unesko-kuriero, 2017-07 &gcirc;is 09 (2)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>falsification</trd>
    <trd>faux<klr> (subst.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="fals.0igi">
  <kap><tld/>igi<fnt>Z</fnt></kap>
  <snc>
    <dif>
      <tld lit="F"/>i.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_r;&c_a;&c_b;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>l&zcaron;i</trd>,
    <trd>p&rcaron;im&ecaron;t k fal&scaron;ov&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="fr">falsifier</trd>
  <trd lng="hu">hamis&iacute;t</trd>
  <trdgrp lng="pl">
    <trd>fa&lstroke;szowa&cacute;</trd>,
    <trd>podrabia&cacute;</trd>,
    <trd>falsyfikowa&cacute;</trd>,
    <trd>preparowa&cacute;<klr> (np. dowody)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>klamstvu</trd>,
    <trd>prin&uacute;ti&tcaron; k fal&scaron;ovaniu</trd>
  </trdgrp>
</drv>

<drv mrk="fals.0ema">
  <kap><tld/>ema<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Malsincera, mensoga.
    </dif>
    <refgrp tip="vid">
      <ref cel="hipokr.0a">hipokrita</ref>,
      <ref cel="jezuit">jezuita</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_ib;&c_l;&c_zh;&c_y;&c_v;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>fale&scaron;n&yacute;</trd>,
    <trd>neup&rcaron;&iacute;mn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>false</trd>,
    <trd>insincere</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fallacieux</trd>,
    <trd>mensonger</trd>,
    <trd>trompeur</trd>
  </trdgrp>
  <trd lng="hu">hamis</trd>
  <trd lng="nl">vals</trd>
  <trdgrp lng="pl">
    <trd>zak&lstroke;amany</trd>,
    <trd>pomawiaj&aogonek;cy</trd>,
    <trd>obmawiaj&aogonek;cy</trd>,
    <trd>krzywoprzysi&eogonek;&zdot;ny</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_zh;&c_i;&c_v;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>falo&scaron;n&yacute;</trd>,
    <trd>ne&uacute;primn&yacute;</trd>
  </trdgrp>
</drv>
</art>
