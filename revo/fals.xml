<drv mrk="fals.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fals.0i.KOMUNE">
    <dif>
      Trompe &scirc;an&gcirc;i, aliigi la staton a&ubreve; konsiston de
      io; trompe imiti la &scirc;ajnon de io:
      <ekz>
        <tld/>ita lakto, vino;
      </ekz>
      <ekz>
        &ccirc;u Dio <tld/>as la ju&gcirc;on
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 8:3</lok>
        </fnt>?
      </ekz>
      <ekz>
        oni vin ripro&ccirc;os kaj persekutos kaj <tld/>e vin kalumnios pro mi
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 5:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>atestoj
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 15:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        <esc>false&jcirc;urantoj</esc>
        <fnt>
          <bib>NT</bib>
          <lok>I. Timoteo 1:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi fariseoj, <tld/>istoj de la vero
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_r;&c_a;&c_b;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>falzifikovat</trd>,
    <trd>fal&scaron;ovat</trd>,
    <trd>pad&ecaron;lat</trd>,
    <trd>podvrhnout</trd>
  </trdgrp>
  <trd lng="de">f&auml;lschen</trd>
  <trdgrp lng="en">
    <trd>falsify</trd>,
    <trd>counterfeit</trd>,
    <trd>fake</trd>,
    <trd>forge</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>falsear</trd>,
    <trd>falsificar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>contrefaire</trd>,
    <trd>falsifier<klr tip="ind"> (contrefaire)</klr></trd>,
    <trd>frelater</trd>
  </trdgrp>
  <trd lng="hu">hamis&iacute;t</trd>
  <trdgrp lng="ja">
    <trd>&#x507D;&#x9020;&#x3059;&#x308B;  <pr>&#x304E;&#x305E;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5909;&#x9020;&#x3059;&#x308B;  <pr>&#x3078;&#x3093;&#x305E;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x507D;&#x4F5C;&#x3059;&#x308B;  <pr>&#x304E;&#x3064;&#x304F;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x6539;&#x7AC4;&#x3059;&#x308B;  <pr>&#x304B;&#x3044;&#x3056;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x507D;&#x308B;  <pr>&#x3044;&#x3064;&#x308F;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">vervalsen</trd>
  <trdgrp lng="pl">
