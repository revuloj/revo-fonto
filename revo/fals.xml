<drv mrk="fals.0ema">
  <kap><tld/>ema, <var><kap><tld/>ama</kap></var></kap>
  <snc mrk="fals.0ema.malsincera">
    <dif>
      Malsincera, mensoga:
      <ekz>
        kiu ka&scirc;as malamon, tiu havas <tld/>eman bu&scirc;on
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 10:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        atestanto <tld/>ama elspiras mensogojn
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 14:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>emo pereigos la maliculojn
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 11:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne sidas kun homoj malveremaj, kaj kun <tld/>emuloj mi ne
        iros
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 26:4</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hipokr.0a">hipokrita</ref>,
      <ref cel="jezuit">jezuita</ref>
    </refgrp>
    <trd lng="de">falsch <klr>(Neigung, Charakter)</klr></trd>
  </snc>
  <trd lng="be">&c_ib;&c_l;&c_zh;&c_y;&c_v;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>fale&scaron;n&yacute;</trd>,
    <trd>neup&rcaron;&iacute;mn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>false</trd>,
    <trd>insincere</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fallacieux</trd>,
    <trd>mensonger</trd>,
    <trd>trompeur</trd>
  </trdgrp>
  <trd lng="hu">hamis</trd>
  <trdgrp lng="ja">
    <trd>&#x4EBA;&#x3092;&#x6B3A;&#x304F;  <pr>&#x3058;&#x3093;&#x3092;&#x3042;&#x3056;&#x3080;&#x304F;</pr></trd>,
    <trd>&#x4E0D;&#x8AA0;&#x5B9F;&#x306A;  <pr>&#x3075;&#x305B;&#x3044;&#x3058;&#x3064;&#x306A;</pr></trd>
  </trdgrp>
  <trd lng="nl">vals</trd>
  <trdgrp lng="pl">
    <trd>zak&lstroke;amany</trd>,
    <trd>pomawiaj&aogonek;cy</trd>,
    <trd>obmawiaj&aogonek;cy</trd>,
    <trd>krzywoprzysi&eogonek;&zdot;ny</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_zh;&c_i;&c_v;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
