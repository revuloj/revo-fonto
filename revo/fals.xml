<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fals</rad>/i
</kap>
<drv mrk="fals.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Trompe &scirc;an&gcirc;i, aliigi la staton a&ubreve; konsiston de io;
      trompe imiti la &scirc;ajnon de io:
      <ekz>
        <tld/>ita lakto, vino;
      </ekz>
      <ekz>
        <tld/>i dokumenton<fnt>B</fnt>, la veron;
      </ekz>
      <ekz>
        oni poste <tld/>is la sencon de miaj vortoj<fnt>Z</fnt>.
      </ekz> 
    </dif>
  </snc>
  <trd lng="de">f&auml;lschen</trd>
  <trd lng="en">falsify</trd>
</drv>
<drv mrk="fals.0a">
  <kap><tld/>a</kap>
  <snc num="1">
    <dif>
      <tld/>ita:
      <ekz>
        <tld/>a dento, hararo
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="natur">nenatura</ref>,
      <ref cel="art">arta&jcirc;a</ref>.
    </refgrp>
  </snc>
  <snc num="2">
    <dif>
      Malvera, nea&ubreve;tentika:
      <ekz>
        <tld/>a aserto, kalkulo, pentra&jcirc;o, subskribo.
      </ekz>
    </dif>
  </snc>
  <snc num="3">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Ne&gcirc;usta:
      <ekz>
        <tld/>a noto;
      </ekz>
      <ekz>
        <tld/>e kanti.
      </ekz>
    </dif>
  </snc>
  <snc num="4">
    <dif>
      <tld/>ema:
      <ekz>
        <tld/>a atestanto, kiu elspiras mensogojn
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Sentencoj 6, 19</lok>)</fnt>;
      </ekz>
      <ekz>
        <tld/>a lango<fnt>Z</fnt>.
      </ekz> 
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>falsch</trd>,
    <trd>gef&auml;lscht</trd>,
    <trd>unecht</trd>
  </trdgrp>
</drv>
<drv mrk="fals.0igi">
  <kap><tld/>igi<fnt>Z</fnt></kap>
  <snc>
    <dif>
      <tld/>i. 
    </dif>
  </snc>
</drv>
<drv mrk="fals.0ema">
  <kap><tld/>ema<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Malsincera, mensoga 
    </dif>
    <refgrp tip="sin">
      <ref cel="hipokr.0a">hipokrita</ref>,
      <ref cel="jezuit">jezuita</ref>.
    </refgrp>
  </snc>
</drv>
</art>
<!--
$Log$
-->
</vortaro>