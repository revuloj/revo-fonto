<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>graten</rad>/i
</kap>
<drv mrk="graten.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Surkovri man&gcirc;a&jcirc;on per panraspita&jcirc;o kun butero, kaj baki &gcirc;isbrune:
      <ekz><tld/>ita terpoma ka&ccirc;o.</ekz>
    </dif>
  </snc>
  <trd lng="ru">&c_o;&c_b;&c_zh;&c_a;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol; &c_v; &c_s;&c_u;&c_h;&c_a;&c_r;&c_ja;&c_h;</trd>
</drv>
<drv mrk="graten.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
   <dif>
     <tld lit="G"/>ita man&gcirc;a&jcirc;o.  
   </dif>
  </snc>
</drv>
</art>
