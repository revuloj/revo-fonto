<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>graten</rad>/i
</kap>
<drv mrk="graten.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Baki man&gcirc;a&jcirc;on, kutime &scirc;mirinte &gcirc;in per pasto,
      pana, froma&gcirc;a a&ubreve;
      alia raspa&jcirc;o, &gcirc;is formi&gcirc;o de delikata krusteto:
      <ekz>
        <tld/>ita terpoma ka&ccirc;o,
      </ekz>
      <ekz>
        makaronioj <tld/>itaj en sa&ubreve;co,
      </ekz>
      <ekz>
        <tld/>ita fi&scirc;o, kokida&jcirc;o ktp.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="pan.0erumi">panerumi</ref>
  <trdgrp lng="be">
    <trd>&c_p;&c_ja;&c_ch;&c_y; <klr>(&c_je;&c_zh;&c_u;)</klr></trd>,
    <trd>&c_z;&c_a;&c_p;&c_ja;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gratinieren</trd>,
    <trd>&uuml;berbacken</trd>
  </trdgrp>
  <trd lng="es">gratinar</trd>
  <trd lng="fr">gratiner</trd>
  <trdgrp lng="hu">
    <trd>cs&odblac;ben <ind>s&uuml;t</ind></trd>,
    <trd>gratin&iacute;roz</trd>
  </trdgrp>
  <trd lng="nl">gratineren</trd>
  <trd lng="pt">preparar comida &agrave; <ind>milanesa</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_p;&c_je;&c_ch;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_je;&c_k;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="graten.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <tld lit="G"/>ita man&gcirc;a&jcirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_p;&c_ja;&c_k;&c_a;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Auflauf</trd>,
    <trd>Gratin</trd>
  </trdgrp>
  <trd lng="fr">gratin</trd>
  <trd lng="hu">cs&odblac;ben <ind>s&uuml;lt</ind></trd>
  <trd lng="nl">gratin</trd>
  <trd lng="pt">comida &agrave; <ind>milanesa</ind></trd>
  <trd lng="ru">&c_z;&c_a;&c_p;&c_je;&c_k;&c_a;&c_n;&c_k;&c_a;</trd>
</drv>
</art>
