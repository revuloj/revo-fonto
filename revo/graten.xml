<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>graten</rad>/i
</kap>
<drv mrk="graten.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Surkovri man&gcirc;a&jcirc;on per panraspita&jcirc;o kun butero, kaj baki &gcirc;isbrune:
      <ekz><tld/>ita terpoma ka&ccirc;o.</ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="pan.0erumi">panerumi</ref>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_p;&c_je;&c_ch;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_je;&c_k;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="graten.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
   <dif>
     <tld lit="G"/>ita man&gcirc;a&jcirc;o.  
   </dif>
  </snc>
  <trd lng="ru">&c_z;&c_a;&c_p;&c_je;&c_k;&c_a;&c_n;&c_k;&c_a;</trd>
</drv>
</art>
