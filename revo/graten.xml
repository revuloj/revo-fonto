<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: graten.xml,v 1.27 2025/04/13 18:35:23 revo Exp $">
<kap>
<rad>graten</rad>/i
<fnt><bib>PVEF</bib></fnt></kap>
<drv mrk="graten.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Baki man&gcirc;a&jcirc;on, kutime &scirc;mirinte &gcirc;in per pasto,
      pana, froma&gcirc;a a&ubreve;
      alia raspa&jcirc;o, &gcirc;is formi&gcirc;o de delikata krusteto:
      <ekz>
        <tld/>ita terpoma ka&ccirc;o,
      </ekz>
      <ekz>
        makaronioj <tld/>itaj en sa&ubreve;co,
      </ekz>
      <ekz>
        <tld/>ita fi&scirc;o, kokida&jcirc;o ktp.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Anka&ubreve; eblas diri <ke>krustbaki</ke>, <ke>krustige baki</ke>, <ke>krustumi</ke>.
  </rim>

  <ref tip="vid" cel="pan.0erumi">panerumi</ref>
  <trdgrp lng="be">
    <trd>&c_p;&c_ja;&c_ch;&c_y; <klr>(&c_je;&c_zh;&c_u;)</klr></trd>,
    <trd>&c_z;&c_a;&c_p;&c_ja;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">gratinar</trd>
  <trdgrp lng="de">
    <trd>gratinieren</trd>,
    <trd>&uuml;berbacken</trd>
  </trdgrp>
  <trd lng="es">gratinar</trd>
  <trd lng="fr">gratiner</trd>
  <trdgrp lng="hu">
    <trd>cs&odblac;ben <ind>s&uuml;t</ind></trd>,
    <trd>gratin&iacute;roz</trd>
  </trdgrp>
  <trd lng="ja">&#12464;&#12521;&#12479;&#12531;&#12395;&#12377;&#12427;  <pr>&#12368;&#12425;&#12383;&#12435;&#12395;&#12377;&#12427;</pr></trd>
  <trd lng="nl">gratineren</trd>
  <trd lng="pt">preparar comida &agrave; <ind>milanesa</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_p;&c_je;&c_ch;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_je;&c_k;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="tr">g&uuml;ve&ccedil;te pi&scedil;irmek </trd>
  <trdgrp lng="sk">
    <trd>zapeka&tcaron;</trd>,
    <trd>zapiec&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>zap&eacute;ci</trd>,
    <trd>zap&eacute;kat</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>grill <klr>(culinary)</klr></trd>,
    <trd>brown <klr>(culinary)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_ib;&c_d;&c_s;&c_m;&c_a;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i; &c_v; &c_s;&c_u;&c_h;&c_a;&c_r;&c_ja;&c_h;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_ib;&c_k;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="graten.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <tld lit="G"/>ita man&gcirc;a&jcirc;o:
      <ekz>
        en <tld/>a&jcirc;oj, supoj, tortoj kaj kukoj, la partoprenantoj malkovris tradiciajn
        a&ubreve; originalajn manierojn man&gcirc;i kukurbon
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470; 12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_p;&c_ja;&c_k;&c_a;&c_n;&c_k;&c_a;</trd>
  <trd lng="ca">menjar gratinat</trd>
  <trdgrp lng="de">
    <trd>Auflauf</trd>,
    <trd>Gratin</trd>
  </trdgrp>
  <trd lng="en">gratin</trd>
  <trd lng="fr">gratin</trd>
  <trd lng="hu">cs&odblac;ben <ind>s&uuml;lt</ind></trd>
  <trd lng="ja">&#12464;&#12521;&#12479;&#12531;</trd>
  <trd lng="nl">gratin</trd>
  <trd lng="pt">comida &agrave; <ind>milanesa</ind></trd>
  <trd lng="ru">&c_z;&c_a;&c_p;&c_je;&c_k;&c_a;&c_n;&c_k;&c_a;</trd>
  <trd lng="tr">g&uuml;ve&ccedil;te pi&scedil;mi&scedil; yiyecek</trd>
  <trd lng="uk">&c_z;&c_a;&c_p;&c_ib;&c_k;&c_a;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#34507;&#30333;&#29275;&#22902;&#37221; <pr>d&agrave;nb&aacute;ini&uacute;n&#462;is&umacron;</pr></trd>,
    <trd>&#37221;&#39173; <pr>s&umacron;b&#464;ng</pr></trd>,
    <trd>&#39201;&#39173; <pr>xi&agrave;nb&#464;ng</pr></trd>
  </trdgrp>
</drv>
</art>
