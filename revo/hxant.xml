<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
 <art mrk="$Id: hxant.xml,v 1.9 2019/03/16 17:10:15 revo Exp $">
 <kap><rad>&hcirc;ant</rad>/o
   <fnt><bib>PIV2</bib>
     <lok>(sub &leftquot;ugra&rightquot;, sen aparta artikolo)</lok>
   </fnt>
 </kap>

<drv mrk="hxant.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ugra gento el &ccirc;. 20&#8239;000 homoj en la okcidenta 
      <ref tip="malprt" cel="siberi.S0o">Siberio</ref>:
      <ekz>
        inter plej &scirc;atataj de la <tld/>oj pladoj estas fi&scirc;rabota&jcirc;o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=29522"
              >&Hcirc;antoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
   <trd lng="be">&c_h;&c_a;&c_n;&c_t;&c_y;</trd>
   <trdgrp lng="de">
     <trd>Chanti</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>Khanty</trd>, <trd>Ostiak</trd>
   </trdgrp>
   <trdgrp lng="fi">
     <trd>hanti</trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>hanti</trd>,
     <trd>osztj&aacute;k</trd>
   </trdgrp>
   <trdgrp lng="it">
     <trd>Khanti</trd>, <trd>Ostiachi</trd>
   </trdgrp>
   <trd lng="nl">Chanti</trd>
   <trdgrp lng="pl">
     <trd>Chantowie</trd>,
     <trd>Ostiacy</trd>
   </trdgrp>
   <trd lng="ru">&c_h;&c_a;&c_n;&c_t;&c_y;</trd>
</drv>

 </art>

