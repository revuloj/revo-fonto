<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>skolopendr</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="skolop1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="skolop1.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Scolopendra</trd>), sudamerikaj miriapodoj,
      ringoformaj, venenaj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Bandassel</trd>,
    <trd>Skolopender</trd>
  </trdgrp>
</drv>
<drv mrk="skolop1.0edoj">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la klaso <ref tip="super" cel="miriap.0oj">miriapodoj</ref>
      (<trd lng="la">Scolopendridae</trd>).
    </dif>
  </snc>
</drv>
</art>
