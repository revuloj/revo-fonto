<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>skolopendr</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="skolop1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super"
      cel="skolop1.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Scolopendra</trd>), sudamerikaj miriapodoj,
      ringoformaj, venenaj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Bandassel</trd>,
    <trd>Skolopender</trd>
  </trdgrp>
  <trd lng="nl">duizendpoot</trd>
  <trd lng="ru">&c_s;&c_k;&c_o;&c_l;&c_o;&c_p;&c_je;&c_n;&c_d;&c_r;&c_a;</trd>
</drv>

<drv mrk="skolop1.0edoj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la klaso <ref tip="super"
      cel="miriap.0oj">miriapodoj</ref>
      (<trd lng="la">Scolopendridae</trd>).
    </dif>
  </snc>
  <trd lng="ru">&c_s;&c_k;&c_o;&c_l;&c_o;&c_p;&c_je;&c_n;&c_d;&c_r;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
