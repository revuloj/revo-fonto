<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>skolopendr</rad>/o
      <fnt>Z</fnt>
      </kap>
    <drv mrk="skolop1.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">ZOO</uzo>
      <snc>
        <dif>Genro el familio 
            <ref tip="super" cel="skolop1.0edoj">
            <tld/>
            edoj</ref>
            (<trd lng="la">Scolopendra</trd>), sudamerikaj miriapodoj, 
            ringoformaj, venenaj.</dif>
      </snc>
      <trd lng="be">&amp;c_s;&amp;c_k;&amp;c_a;&amp;c_l;&amp;c_ja;&amp;
c_p;&amp;c_e;&amp;c_n;&amp;c_d;&amp;c_r;&amp;c_a;</trd>
      <trdgrp lng="de">
         <trd>Bandassel</trd>, 
         <trd>Skolopender</trd>
         </trdgrp>
      <trd lng="hu">szkolopendra</trd>
      <trd lng="pl">skolopendra</trd>
      <trd lng="nl">duizendpoot</trd>
      <trd lng="ru">&amp;c_s;&amp;c_k;&amp;c_o;&amp;c_l;&amp;c_o;&amp;
c_p;&amp;c_je;&amp;c_n;&amp;c_d;&amp;c_r;&amp;c_a;</trd>
    </drv>
    <drv mrk="skolop1.0edoj">
      <kap>
         <tld/>oj</kap>
      <uzo tip="fak">ZOO</uzo>
      <snc>
        <dif>Familio el la klaso 
            <ref tip="super" cel="miriap.0oj">miriapodoj</ref>
            (<trd lng="la">Scolopendridae</trd>).
        </dif>
      </snc>
      <trd lng="be">&amp;c_s;&amp;c_k;&amp;c_a;&amp;c_l;&amp;c_ja;&amp;
c_p;&amp;c_e;&amp;c_n;&amp;c_d;&amp;c_r;&amp;c_y; 
         <klr tip="amb">(&amp;c_s;&amp;c_ja;&amp;c_m;'&amp;c_ja;)</klr>
      </trd>
      <trd lng="hu">szkolopendraflk</trd>
      <trd lng="pl">jednoparce</trd>
      <trd lng="ru">&amp;c_s;&amp;c_k;&amp;c_o;&amp;c_l;&amp;c_o;&amp;
c_p;&amp;c_je;&amp;c_n;&amp;c_d;&amp;c_r;&amp;c_y; 
         <klr tip="amb">(&amp;c_s;&amp;c_je;&amp;c_m;&amp;c_je;&amp;
c_j;&amp;c_s;&amp;c_t;&amp;c_v;&amp;c_o;)</klr>
      </trd>
    </drv>
  </art>
