<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: regxistr.xml,v 1.9 2015/08/30 05:10:16 revo Exp $">
<kap><rad>re&gcirc;istr</rad>/o</kap>
<drv mrk="regxistr.0o">
  <kap><tld/>o <fnt><bib>KompLeks</bib></fnt></kap>
  <snc>
  <uzo tip="fak">KOMP</uzo>
    <dif>
      Peco de la interna
      <ref tip="super" cel="memor.0o.KOMP">memoro</ref> de
      komputilo a&ubreve; adaptilo, normale havanta la kapaciton de unu
      memorvorto, uzata por diversaj specialaj celoj (precipe por
      teni la kurantan valoron de argumento de operacio a&ubreve; &gcirc;ian
      adreson):
      <ekz>
        subtrahi, multobligi, a&ubreve; dividi la valorojn de du
        <tld/>oj, metante la rezulton en <tld/>on
        <fnt><bib>Viki</bib>,
          <lok>La artikolo
            &leftquot;<url ref="https://eo.wikipedia.org/wiki/Asembla_lingvo"
                  >Asembla lingvo</url>&rightquot;</lok>
        </fnt>;
      </ekz><ekz>
        <ind>aritmetika <tld/>o</ind>,
        <trd lng="hu">aritmetikai <ind>register</ind></trd>
        <trd lng="ru">&c_a;&c_r;&c_i;&c_f;&c_m;&c_je;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; <ind>&c_r;&c_je;&c_g;&c_i;&c_s;&c_t;&c_r;</ind></trd>
      </ekz><ekz>
        <ind>baz<tld/>o</ind>,
        <trd lng="hu">b&aacute;zisregister</trd>
        <trd lng="ru">&c_b;&c_a;&c_z;&c_o;&c_v;&c_y;&c_j; <ind>&c_r;&c_je;&c_g;&c_i;&c_s;&c_t;&c_r;</ind></trd>
      </ekz><ekz>
        <ind>indic<tld/>o</ind>.
        <trd lng="hu">indexregister</trd>
        <trd lng="ru">&c_i;&c_n;&c_d;&c_je;&c_k;&c_s;-&c_r;&c_je;&c_g;&c_i;&c_s;&c_t;&c_r;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_g;&c_ib;&c_s;&c_t;&c_r; <klr>(&c_u; &c_k;&c_a;&c_m;&c_p;&c_u;&c_t;&c_a;&c_r;&c_n;&c_a;&c_j; &c_p;&c_a;&c_m;&c_ja;&c_c;&c_ib;)</klr></trd>
  <trd lng="en">register</trd>
  <trd lng="es">registro<klr> (memoria del microprocesador)</klr></trd>
  <trd lng="fr">registre<klr> (de microprocesseur)</klr></trd>
  <trd lng="hu">regiszter</trd>
  <trd lng="nl">register</trd>
  <trd lng="pt">registro</trd>
  <trd lng="ru">&c_r;&c_je;&c_g;&c_i;&c_s;&c_t;&c_r;</trd>
</drv>
</art>
