<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hoke.xml,v 1.25 2023/11/15 17:08:01 revo Exp $">
<kap>
  <rad>hoke</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="hoke.0o">
  <kap><tld/>o</kap>
  <snc mrk="hoke.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Pilka ludo, &ccirc;e kiu la pilko estas pu&scirc;ata kaj batata
      per specialaj kurbaj bastonoj t.n. <tld/>iloj:
      <ekz>
        la regularo de <tld/>o similas tiun de la piedpilka ludo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_k;&c_je;&c_j;</trd>
  <trd lng="bg">&c_h;&c_o;&c_k;&c_je;&c_j;</trd>
  <trd lng="cs">hokej</trd>
  <trd lng="de">Hockey</trd>
  <trd lng="el">&chi;&omikron_ton;&kappa;&jota;</trd>
  <trd lng="en">hockey</trd>
  <trd lng="es">hockey</trd>
  <trd lng="fa">&a_h;&a_A;&f_k;&f_y;</trd>
  <trd lng="fr">hockey</trd>
  <trdgrp lng="hu">
    <trd>hoki</trd>,
    <trd>j&eacute;korong</trd>,
    <trd>gyeplabda</trd>
  </trdgrp>
  <trd lng="it">hockey</trd>
  <trd lng="ja">&#x30DB;&#x30C3;&#x30B1;&#x30FC;</trd>
  <trd lng="nl">hockey</trd>
  <trd lng="pl">hokej</trd>
  <trd lng="pt">h&oacute;quei</trd>
  <trd lng="ru">&c_h;&c_o;&c_k;&c_k;&c_je;&c_j;</trd>
  <trd lng="sk">hokej</trd>
  <trd lng="uk">&#x433;&#x43E;&#x43A;&#x435;&#x439;</trd>
  <trd lng="zh">&#x66F2;&#x68CD;&#x7403; <pr>q&#x16B;g&#xF9;nqi&#xFA;</pr></trd>
</drv>

<drv mrk="hoke.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="hoke.0ilo.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Speco de kurba bastono uzata &ccirc;e la <tld/>a ludo.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_ju;&c_sh;&c_k;&c_a; <klr tip="amb">(&c_h;&c_a;&c_k;&c_je;&c_j;&c_n;&c_a;&c_ja;)</klr></trd>
  <trd lng="bg">&c_s;&c_t;&c_i;&c_k; &c_z;&c_a;
  &c_h;&c_o;&c_k;&c_je;&c_j;</trd>
  <trd lng="cs">hokejka</trd>
  <trd lng="de">Hockeyschl&auml;ger</trd>
  <trd lng="en">hockey stick</trd>
  <trd lng="fa">&f_ch;&a_w;&a_b; <ind>&a_h;&a_A;&f_k;&f_y;</ind></trd>
  <trd lng="fr">crosse<klr tip="ind"> (de hockey)</klr></trd>
  <trd lng="hu">hoki&uuml;t&odblac;</trd>
  <trd lng="it">bastone da <ind>hockey</ind></trd>
  <trd lng="ja">&#x30B9;&#x30C6;&#x30A3;&#x30C3;&#x30AF;</trd>
  <trd lng="nl">hockeystick</trd>
  <trd lng="pl">kij <ind>hokejowy</ind></trd>
  <trd lng="pt">estique<klr tip="amb"> (de h&oacute;quei)</klr></trd>
  <trd lng="ru">&c_k;&c_l;&c_ju;&c_sh;&c_k;&c_a;
  <klr>(&c_h;&c_o;&c_k;&c_k;&c_je;&c_j;&c_n;&c_a;&c_ja;)</klr></trd>
  <trd lng="sk">hokejka</trd>
  <trd lng="uk">&#x43A;&#x43B;&#x44E;&#x447;&#x43A;&#x430;</trd>
  <trd lng="zh">&#x66F2;&#x68CD; <pr>q&#x16B;g&#xF9;n</pr></trd>
</drv>

<drv mrk="hoke.glaci0o">
  <kap>
    glaci<tld/>o
  </kap>
  <snc mrk="hoke.glaci0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="super" cel="sport.vintra_0o.SPO">Vintra sporto</ref>,
      <tld/>o ludata sur glacia areno:
       <ekz>
        la du koreioj formos komunan teamon por la konkurso de virina glaci<tld/>o
        <fnt>
          <aut>Yu</aut>
          <vrk><url ref="&CRI;2521/2018/02/10/179s197423.htm">Malfermigis 23aj Vintraj Olimpikoj en PyeongChang, Sud-Koreio</url></vrk>
          <lok>&Ccirc;ina Radio Internacia, 2018-02-10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">hockey sur glace</trd>
  <trd lng="ja">&#x30A2;&#x30A4;&#x30B9;&#x30DB;&#x30C3;&#x30B1;&#x30FC;  <pr>&#x3042;&#x3044;&#x3059;&#x307B;&#x3063;&#x3051;&#x30FC;</pr></trd>
  <trd lng="zh">&#x51B0;&#x7403; <pr>b&#x12B;ngqi&#xFA;</pr></trd>
</drv>
</art>
