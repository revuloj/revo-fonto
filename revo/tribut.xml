<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tribut.xml,v 1.20 2024/02/04 01:29:03 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>tribut</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tribut.0o">
  <kap><tld/>o</kap>
  <snc mrk="tribut.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Tio, kion devas periode pagi a&ubreve; liveri iu regno al alia kiel
      signo de dependeco:
      <ekz>
        li dividis la guberniojn kaj difinis la
        <tld/>on, kiun ili devis pagi &ccirc;iujare
        <fnt>
          <aut>Herodoto, trad. S. Sarafian</aut>,
          <vrk>Historio</vrk>,
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>o de admiro, de respekto.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kontri1.0o.POL">kontribucio</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_n;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>clo</trd>,
    <trd>da&ncaron;</trd>,
    <trd>hold</trd>,
    <trd>poplatek</trd>,
    <trd>poplatek z&#xE1;vislosti</trd>,
    <trd>tribut</trd>,
    <trd>&#x10D;est</trd>
  </trdgrp>
  <trd lng="de">Tribut</trd>
  <trd lng="en">tribute</trd>
  <trd lng="fr">tribut</trd>
  <trd lng="hu">h&udblac;b&eacute;rad&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x8CA2;&#x304E;&#29289;  <pr>&#x307F;&#12388;&#x304E;&#12418;&#x306E;</pr></trd>,
    <trd>&#x671D;&#x8CA2;  <pr>&#12385;&#12423;&#12358;&#12371;&#12358;</pr></trd>,
    <trd>&#x5C0A;&#x656C;&#x306E;&#21360;  <pr>&#x305D;&#12435;&#12369;&#12356;&#x306E;&#12356;&#12435;</pr></trd>,
    <trd>&#12373;&#12373;&#12370;&#29289;  <pr>&#12373;&#12373;&#12370;&#12418;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">schatting</trd>
  <trdgrp lng="pl">
    <trd>trybut</trd>,
    <trd>czynsz</trd>,
    <trd>danina</trd>,
    <trd>haracz</trd>
  </trdgrp>
  <trd lng="pt">tributo</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_a;&c_n;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>da&ncaron;</trd>,
    <trd>poplatok</trd>,
    <trd>trib&#xFA;t</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_a;&#x43D;&#x44C;</trd>,
    <trd>&c_d;&c_a;&#x43D;&c_i;&#x43D;&c_a;</trd>,
    <trd>&#x43F;&#x43E;&c_d;&c_a;&c_t;&#x44C;</trd>,
    <trd>&#x43F;&#x43E;&c_d;&c_a;&c_t;&#x43E;&#x43A;</trd>
  </trdgrp>
</drv>

<drv mrk="tribut.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="tribut.0i.POL">
    <dif>
      <tld lit="T"/>e liveri:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>u al Dio gloron kaj potencon<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_a;&c_c;&c_ib;&c_c;&c_mol;
    <ind>&c_d;&c_a;&c_n;&c_ib;&c_n;&c_u;</ind></trd>
  <trdgrp lng="cs">
    <trd>platit da&ncaron;</trd>,
    <trd>platit tribut</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>entrichten</trd>,
    <trd>Tribut zollen</trd>
  </trdgrp>
  <trd lng="en">pay tribute</trd>
  <trd lng="fr">payer le tribut</trd>
  <trd lng="hu">h&udblac;b&eacute;rad&oacute;t fizet</trd>
  <trdgrp lng="ja">
    <trd>&#x8CA2;&#12368;  <pr>&#x307F;&#12388;&#12368;</pr></trd>,
    <trd>&#x8CA2;&#x7D0D;&#12377;&#x308B;  <pr>&#x307F;&#12388;&#12368;&#x7D0D;&#12377;&#x308B;</pr></trd>,
    <trd>&#x671D;&#x8CA2;&#12377;&#x308B;  <pr>&#12354;&#12373;&#x307F;&#12388;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">schatting betalen</trd>
  <trdgrp lng="pl">
    <trd>p&lstroke;aci&cacute;  <ind>danin&eogonek;</ind></trd>,
    <trd>p&lstroke;aci&cacute;  <ind>haracz</ind></trd>
  </trdgrp>
  <trd lng="pt">tributar</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_t;&c_i;&c_t;&c_mol; <ind>&c_d;&c_a;&c_n;&c_mol;</ind></trd>
  <trd lng="sk">plati&tcaron; da&ncaron;</trd>
  <trd lng="uk">&c_s;&#x43F;&#x43B;&c_a;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i; &c_d;&c_a;&#x43D;&c_i;&#x43D;&c_u;</trd>
</drv>
</art>
