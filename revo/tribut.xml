<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>tribut</rad>/o
</kap>
<drv mrk="tribut.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Tio, kion devas periode pagi a&ubreve; liveri iu regno al alia kiel signo
      de dependeco:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>o de admiro, de respekto.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Tribut</trd>
  <trd lng="nl">schatting</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_a;&c_n;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="tribut.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="T"/>e liveri:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>u al Dio gloron kaj potencon<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="impost.0o">imposto</ref>,
      <ref cel="kontri1.0o">kontribucio</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">schatting betalen</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_t;&c_i;&c_t;&c_mol; <ind>&c_d;&c_a;&c_n;&c_mol;</ind></trd>
</drv>
</art>
