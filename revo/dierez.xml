<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <rad>dierez</rad>/o
  </kap>
  <drv mrk="dierez.0o">
    <kap><tld/>o</kap>
    <snc mrk="dierez.0o.FON">
      <uzo tip="fak">FON</uzo>
      <dif>
        Disa prononco de vokaloj, kontraste al la diftonga:
        <ekz>
           en la vortoj <em>praulo, Raulo, neuzata</em>
           la kombinoj <em>au, eu</em> formas <tld/>on
           <klr>(ne kombini&gcirc;as en diftongojn
           <em>a&ubreve;, e&ubreve;</em>)</klr>.
       </ekz>
      </dif>
      <ref tip="ant" cel="difton.0o">diftongo</ref>
      <trd lng="ca">hiat</trd>
      <trd lng="de">Di&auml;rese</trd>
      <trd lng="es">di&eacute;resis</trd>
      <trd lng="fr">di&eacute;r&egrave;se</trd>
      <trdgrp lng="hu">
        <trd>dier&eacute;zis</trd>,
        <trd>k&uuml;l&ouml;nejt&eacute;s</trd>
      </trdgrp>
      <trd lng="pl">diereza</trd>
      <trd lng="pt">di&eacute;rese</trd>
    </snc>
    <snc mrk="dierez.0o.tremao">
      <uzo tip="stl">EVI</uzo>
      <ref tip="dif" cel="trema.0o">Tremao</ref>
      <trd lng="ca">di&egrave;resi</trd>
      <trdgrp lng="de">
        <trd>Trema</trd>,
        <trd>Trennungspunkte</trd>,
        <trd>Umlautpunkte</trd>
      </trdgrp>
      <trd lng="fr">tr&eacute;ma</trd>
      <trd lng="hu">tr&eacute;ma</trd>
      <trdgrp lng="pl">
        <trd>diereza<klr> (znak diakrytyczny)</klr></trd>,
        <trd>tr&eacute;ma</trd>
      </trdgrp>
    </snc>
  <trd lng="ru">&c_d;&c_i;&c_je;&c_r;&c_je;&c_z;&c_a;</trd>
  <trd lng="sk">osobitné vyslovenie samohlások u dvojhlások</trd>
</drv>
</art>
