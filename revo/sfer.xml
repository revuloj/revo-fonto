<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>sfer</rad>/o
</kap>
<drv mrk="sfer.0o">
  <kap><tld/>o</kap>
  <snc mrk="sfer.0o.surfaco">
    <uzo tip="fak">MAT</uzo>
    <dif>
      Trimensia <ref tip="super" cel="surfac.0o.MAT">surfaco</ref>,
      konsistanta el
      &ccirc;iuj punktoj, kies distanco al certa punkto <klr>(&gcirc;ia
      centro)</klr> egalas al certa valoro <klr>(&gcirc;ia radiuso)</klr>.
    </dif>
    <refgrp tip="vid">
      <ref cel="centr.0o.cirklo">centro</ref>,
      <ref cel="diamet.0o.cirklo">diametro</ref>,
      <ref cel="radius.0o.cirklo">radiuso</ref>.
    </refgrp>
  </snc>
  <snc mrk="sfer.0o.solido">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <ref tip="super" cel="solid.0o.geometrio">Solido</ref>, kiun
      limas <tld/>o <sncref ref="sfer.0o.surfaco"/>.
    </dif>
    <refgrp tip="sin">
      <ref cel="glob.0o.sfero">globo</ref>.
    </refgrp>
  </snc>
  <snc mrk="sfer.0o.KOMUNE">
    <dif>
      &Ccirc;ia objekto pli-malpli samforma kiel <tld/>o
      <sncref ref="sfer.0o.surfaco"/> a&ubreve; <sncref ref="sfer.0o.solido"/>:
      <ekz>
        la <tld/>o &ccirc;iela<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bul.0o">bulo</ref>,
      <ref cel="glob.0o">globo</ref>,
      <ref cel="rond.0o">rondo</ref>.
    </refgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Medio; kampo de agado:
      <ekz>
        &ccirc;iu povas agadi aparte en sia <tld/>o,
        la&ubreve; siaj fortoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj <tld/>oj de la homa societo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        bone konata en <tld/>oj pedagogaj lingvisto<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rond.0o">rondo</ref>,
      <ref cel="region.0o">regiono</ref>,
      <ref cel="teren.0o">tereno</ref>.
    </refgrp>
  </snc>
  <trd lng="en">sphere</trd>
  <trd lng="fr">sph&egrave;re</trd>
</drv>
<drv mrk="sfer.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>aj koordinatoj;
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Havanta formon de <tld/>o:
      <ekz>
        <tld/>a kapo;
      </ekz>
      <ekz>
        duon<tld/>a kupolo.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">sph&eacute;rique</trd>
</drv>
</art>
