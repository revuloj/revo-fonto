<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sfer.xml,v 1.47 2024/03/24 20:21:56 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>sfer</rad>/o <fnt><bib>DL</bib></fnt>
</kap>

<drv mrk="sfer.0o">
  <kap><tld/>o</kap>
  <snc mrk="sfer.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="sfer.0o.surfaco">
      <fnt><bib>MatTerm</bib><lok>p. 30</lok></fnt>
      <dif>
        Tridimensia
        <ref tip="super" cel="surfac.0o.MAT">surfaco</ref>,
        konsistanta el &ccirc;iuj punktoj, kies distanco al iu punkto
        <klr>(&gcirc;ia centro)</klr> egalas al iu valoro
        <klr>(&gcirc;ia radiuso)</klr>:
        <ekz>
          la areo de <tld/>o havanta la radiuson <frm am="r">r</frm>
          estas <frm am="4 pi r^2">4&pi;r&quadrat;</frm>;
        </ekz>
        <ekz>
          <ref tip="malprt" cel="glob.0o.sfero">globo<sncref/></ref>
          estas
          <ref tip="super" cel="solid.0o.geometrio">solido</ref>
          limata de <tld/>o.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="centr.0o.cirklo">centro</ref>,
        <ref cel="diamet.0o.cirklo">diametro</ref>,
        <ref cel="radius.0o.cirklo">radiuso</ref>;
      </refgrp>
      <refgrp tip="prt">
        <ref cel="zon.0o.MAT">zono</ref>,
        <ref cel="cxap.verto0o.MAT">verto&ccirc;apo</ref>,
        <ref cel="kalot.0o">kaloto</ref>.
      </refgrp>
      <trd lng="be">&c_s;&c_f;&c_e;&c_r;&c_a;</trd>
      <trdgrp lng="de">
        <trd>Kugeloberfl&auml;che</trd>,
        <trd>Sph&auml;re</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>g&ouml;mbh&eacute;j</trd>,
        <trd>g&ouml;mbfel&uuml;let</trd>
      </trdgrp>
      <trd lng="pl">sfera</trd>
      <trd lng="ru">&c_s;&c_f;&c_je;&c_r;&c_a;</trd>
    </subsnc>
    <subsnc mrk="sfer.0o.solido">
      <fnt><bib>VdE</bib></fnt>
      <dif>
        <ref tip="dif" cel="glob.0o.sfero">Globo<sncref/></ref>.
      </dif>
      <refgrp tip="prt">
        <ref cel="segmen.0o.sfero">segmento</ref>,
        <ref cel="tavol.glob0o">globtavolo</ref>,
        <ref cel="disk.sfer0o"><tld/>disko</ref>.
      </refgrp>
      <trdgrp lng="be">
        <trd>&c_sh;&c_a;&c_r;</trd>,
        <trd>&c_k;&c_u;&c_l;&c_ja;</trd>
      </trdgrp>
      <trd lng="de">Kugel</trd>
      <trd lng="hu">g&ouml;mb</trd>
      <trdgrp lng="en">
        <trd>sphere</trd>,
        <trd>ball<klr tip="ind"> (solid)</klr></trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>sph&egrave;re</trd>,
        <trd>boule<klr tip="ind"> (volume sph&eacute;rique)</klr></trd>
      </trdgrp>
      <trd lng="pl">kula</trd>
      <trd lng="ru">&c_sh;&c_a;&c_r;</trd>
    </subsnc>
  </snc>
  <snc mrk="sfer.0o.KOMUNE">
    <dif>
      &Ccirc;ia objekto pli-malpli samforma kiel <tld/>o
      <sncref ref="sfer.0o.surfaco"/> a&ubreve; <sncref ref="sfer.0o.solido"/>:
      <ekz>
        la <ind><tld/>o &ccirc;iela</ind>
        <fnt>
          <bib>FK</bib>
        </fnt>.
        <trd lng="es">esfera celeste</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bul.0o">bulo</ref>,
      <ref cel="glob.0o">globo</ref>,
      <ref cel="rond.0o">rondo</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_s;&c_f;&c_e;&c_r;&c_a;</trd>,
      <trd>&c_sh;&c_a;&c_r;</trd>,
      <trd>&c_k;&c_u;&c_l;&c_ja;</trd>
    </trdgrp>
    <trd lng="ca">bola</trd>
    <trdgrp lng="hu">
      <trd>g&ouml;mb</trd>,
      <trd>gl&oacute;busz</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_f;&c_je;&c_r;&c_a;</trd>,
      <trd>&c_sh;&c_a;&c_r;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sfer.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="vid" cel="medi.0o">Medio</ref>;
      kampo de agado:
      <ekz>
        mia konanto estas en la altaj <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 16:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        ilia vo&ccirc;o estis kiel sonorado de <klr>(&ccirc;ielaj)</klr> <tld/>oj, sed tiel spirita, ke nenia
        homa orelo
        povis &gcirc;in a&ubreve;di, kiel anka&ubreve; nenia tera okulo povis vidi tiujn &ccirc;i &ccirc;ielajn
        esta&jcirc;ojn
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj <tld/>oj de la homa societo
        <fnt>
          <bib>EE</bib>
        </fnt>
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        bone konata en <tld/>oj pedagogaj lingvisto
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        mallevi&gcirc;o al tiel malaltaj <tld/>oj estu &scirc;ia sorto
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        detran&ccirc;ite de la patro kaj de la hejma <tld/>o, &scirc;i per si
        mem faris ion plian kaj translo&gcirc;i&gcirc;is al Istanbulo
        <fnt>
          <aut>B. Strau&szlig;, trad. V. Lutermano</aut>
          <vrk><url ref="http://vlutermano.free.fr/pramo-cx.html">
          La virino sur la pramo</url></vrk>
          <lok>2001</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="region.0o">regiono</ref>,
      <ref cel="rond.0o">rondo</ref>,
      <ref cel="teren.0o">tereno</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_s;&c_f;&c_e;&c_r;&c_a;</trd>,
      <trd>&c_p;&c_o;&c_l;&c_je; <klr tip="amb">(&c_d;&c_z;&c_je;&c_j;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>camp</trd>,
      <trd>&agrave;rea</trd>
    </trdgrp>
    <trd lng="de">Sph&auml;re</trd>
    <trd lng="hu">szf&eacute;ra</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_f;&c_je;&c_r;&c_a;</trd>,
      <trd>&c_p;&c_o;&c_l;&c_je; &c_d;&c_je;&c_ja;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_i;</trd>
    </trdgrp>
  </snc>
  <trd lng="ca">esfera</trd>
  <trdgrp lng="cs">
    <trd>koule</trd>,
    <trd>kulov&aacute; plocha</trd>,
    <trd>okruh</trd>,
    <trd>sf&eacute;ra</trd>
  </trdgrp>
  <trd lng="en">sphere</trd>
  <trd lng="es">esfera</trd>
  <trd lng="fr">sph&egrave;re</trd>
  <trd lng="it">sfera</trd>
  <trdgrp lng="ja">
    <trd>&#x7403;  <pr>&#x305F;&#x307E;</pr></trd>,
    <trd>&#x7403;&#x9762;  <pr>&#x304D;&#x3085;&#x3046;&#x3081;&#x3093;</pr></trd>,
    <trd/>,
    <trd>&#x8ECC;&#x9053;  <pr>&#x304D;&#x3069;&#x3046;</pr></trd>,
    <trd>&#x9818;&#x57DF;  <pr>&#x308A;&#x3087;&#x3046;&#x3044;&#x304D;</pr></trd>,
    <trd>&#x7BC4;&#x56F2;  <pr>&#x306F;&#x3093;&#x3044;</pr></trd>,
    <trd>&#x570F;  <pr>&#x3051;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">sfeer</trd>
  <trd lng="pt">esfera</trd>
  <trdgrp lng="sk">
    <trd>gu&lcaron;a</trd>,
    <trd>okruh</trd>,
    <trd>sf&eacute;ra</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_f;&c_je;&c_r;&c_a;</trd>,
    <trd>&c_s;&c_je;&c_r;&c_je;&c_d;&#x43E;&c_v;&c_i;&c_shch;&c_je;</trd>,
    <trd>&#x43F;&#x43E;&#x43B;&c_je; &c_d;&c_ib;&#x44F;&#x43B;&#x44C;&#x43D;&#x43E;&c_s;&c_t;&c_ib;</trd>,
    <trd>&#x43A;&c_u;&#x43B;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5706;&#x7403; <pr>yu&#xE1;nqi&#xFA;</pr></trd>,
    <trd>&#x5713;&#x7403; <pr>yu&#xE1;nqi&#xFA;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sfer.0a">
  <kap><tld/>a</kap>
  <snc mrk="sfer.0a.rilata">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        la situacio <tld/>a de la suno
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>aj koordinatoj</ind>.
        <trd lng="es">coordenadas esf&eacute;ricas</trd>
      </ekz>
    </dif>
  </snc>
  <snc mrk="sfer.0a.havanta">
    <dif>
      Havanta formon de <tld/>o:
      <ekz>
        <tld/>a kapo;
      </ekz>
      <ekz>
        <ind>duon<tld/>a kupolo</ind>.
        <trd lng="hu">f&eacute;lg&ouml;mb kupola</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_f;&c_e;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">esf&egrave;ric</trd>
  <trdgrp lng="cs">
  <trd>kulov&yacute;</trd>,
  <trd>sf&eacute;rick&yacute;</trd>
  </trdgrp>
  <trd lng="de">sph&auml;risch</trd>
  <trd lng="es">esf&eacute;rico</trd>
  <trd lng="fr">sph&eacute;rique</trd>
  <trdgrp lng="hu">
      <trd>g&ouml;mb-</trd>,
      <trd>g&ouml;mb&ouml;ly&udblac;</trd>
      <trd>g&ouml;mb alak&uacute;</trd>
    </trdgrp>
  <trd lng="it">sferico</trd>
  <trdgrp lng="ja">
    <trd>&#x7403;&#x9762;&#x306E;  <pr>&#x305F;&#x307E;&#x304A;&#x3082;&#x3066;&#x306E;</pr></trd>,
    <trd>&#x7403;&#x306E;  <pr>&#x305F;&#x307E;&#x306E;</pr></trd>,
    <trd>&#x7403;&#x72B6;&#x306E;  <pr>&#x305F;&#x307E;&#x3058;&#x3087;&#x3046;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>sferisch</trd>,
    <trd>bolvormig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kulisty</trd>,
    <trd>okr&aogonek;g&lstroke;y</trd>,
    <trd>sferyczny</trd>
  </trdgrp>
  <trd lng="pt">esf&eacute;rico</trd>
  <trd lng="ru">&c_s;&c_f;&c_je;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
  <trd>gu&lcaron;ov&yacute;</trd>,
  <trd>sf&eacute;rick&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_f;&c_je;&c_r;&c_i;&c_ch;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43A;&c_u;&#x43B;&#x44F;&c_s;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7403;&#x4F53; <pr>qi&#xFA;t&#x1D0;</pr></trd>,
    <trd>&#x7403;&#x9AD4; <pr>qi&#xFA;t&#x1D0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sfer.duon0o">
  <kap>duon<tld/>o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="sfer.duon0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <dif>
      &Ccirc;iu el la du partoj de <ref cel="sfer.0o.surfaco"><tld/>o<sncref/></ref>,
      apartigitaj per ebeno trapasanta &gcirc;ian centron.
    </dif>
    <ref tip="sin" cel="hemisf.0o.MAT">hemisfero</ref>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_s;&c_f;&c_je;&c_r;&c_a;</trd>
  </snc>
  <snc mrk="sfer.duon0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      &Ccirc;iu el la du partoj de la tersurfaco, dividitaj per la
      ekvatoro a&ubreve; meridiano:
      <ekz>
        la tera ekvatoro dividas la teron en du duon<tld/>ojn: la
        nordan kaj la sudan<fnt><bib>VdE</bib></fnt>;
      </ekz><ekz>
        la Ekvatoro de Tero estas imaga ronda linio, egaldistanca
        al la norda kaj suda polusoj, kiu dividas la tersurfacon
        en nordan kaj sudan duon<tld/>ojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="&Viki;/Ekvatoro">
          Ekvatoro</url>, 2013-04-03</lok>
        </fnt>;
      </ekz><ekz>
        <klr>[San-Pa&ubreve;lo]</klr> estas la plej grava ekonomia
        centro kaj plej granda urbo kaj de Brazilo kaj de la
        suda duon<tld/>o entute
        <fnt>
          <lok><url ref="http://esperanto.televido.eo.infofx.net">
          San-Pa&ubreve;lo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        bondezirojn por la nova jaro anka&ubreve; el la okcidenta
        duon<tld/>o!
        <fnt>
          <aut>D. Harlow</aut>
          <vrk><url ref="http://groups.yahoo.com/group/aeko/message/784">
          NovJarajn salutojn al &ccirc;iuj!</url></vrk>
          <lok>2002-01-01</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="glob.duon0o">duonglobo</ref>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_sh;&c_a;&c_r;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="sfer.duon0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      &Ccirc;iu el la du partoj de la
      <ref tip="malprt" cel="cxiel1.0osfero">&ccirc;ielo<tld/>o</ref>,
      apartigitaj per la ekvatora ebeno:
      <ekz>
        kelkaj steloj komencis lumi, kaj se la in&gcirc;eniero estus
        kun siaj kamaradoj, li certe rimarkus, ke tiuj &ccirc;i steloj
        ne apartenis al la norda duon<tld/>o
        <fnt>
          <aut>J. Verne, trad. J. Wa&lstroke;aszek</aut>:
          <vrk>La mistera insulo</vrk>.
          <lok><url ref="http://www.i-lo.tarnow.pl/edu/esp/lern/jv_mi/pages/003.php">
          &Ccirc;apitro 3a</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_sh;&c_a;&c_r;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="sfer.duon0o.duonglobo">
    <dif>
      <ref tip="dif" cel="glob.duon0o">Duonglobo</ref>:
      <ekz>
        la du cerbaj duon<tld/>oj:
        <fnt><bib>BonaLingvo</bib>, <lok>&ccirc;apitro 2a</lok></fnt>
      </ekz>
      <ekz>
        nia dekstra cerba duon<tld/>o enhavas, en sia paradiza parto, grandegan
        potencialon da plezuro
        <fnt>
          <aut>Claude Piron</aut>
          <vrk><url ref="https://medium.com/esperantoperm/claude-piron-esperanto-kaj-ties-perspektivoj-in-2005-abeb5887c16b">Esperanto
          kaj ties perspektivoj in 2005</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_sh;&c_a;&c_r;&c_i;&c_je;</trd>
  </snc>
  <adm>
    Kontroli trd de/en. <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_w;&c_s;&c_f;&c_e;&c_r;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_w;&c_sh;&c_a;&c_r;'&c_je;</trd>,
    <trd>&c_p;&c_a;&c_w;&c_k;&c_u;&c_l;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>semiesfera</trd>,
    <trd>hemisferi</trd>
  </trdgrp>
  <trd lng="de">Halbkugel</trd>
  <trd lng="el">&eta;&my;&jota;&sigma;&phi;&alfa;&jota_ton;&rho;&jota;&omikron;</trd>
  <trdgrp lng="en">
    <trd>hemisphere</trd>,
    <trd>half sphere</trd>
  </trdgrp>
  <trd lng="es">semiesfera</trd>
  <trd lng="fr">h&eacute;misph&egrave;re</trd>
  <trd lng="ja">&#x534A;&#x7403;  <pr>&#x306F;&#x3093;&#x304D;&#x3085;&#x3046;</pr></trd>
  <trd lng="pl">p&oacute;&lstroke;sfera</trd>
  <trd lng="pt">semi-esfera</trd>
  <trd lng="sk">pologu&lcaron;a</trd>
  <trdgrp lng="cs">
  <trd>hemisf&eacute;ra</trd>,
  <trd>polosf&eacute;ra</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&c_ib;&c_v;&#x43A;&c_u;&#x43B;&#x44F;</trd>
  <trd lng="zh">&#x534A;&#x7403; <pr>b&#xE0;nqi&#xFA;</pr></trd>
</drv>

<drv mrk="sfer.duon0a">
  <kap>duon<tld/>a</kap>
  <snc mrk="sfer.duon0a.formo">
    <dif>
      Havanta la formon de duon<tld/>o:
      <ekz>
        kupolo <klr>[estas]</klr> duon<tld/>a konstruo en formo de
        renversita kaliko
        <fnt><bib>PIV2</bib> <lok>art. &#xAB;kupolo&#xBB;</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_s;&c_f;&c_je;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; </trd>
    <trd lng="nl">half <ind>rond</ind></trd>
    <trd lng="pt">hemisf&eacute;rico</trd>
  </snc>
  <snc mrk="sfer.duon0a.rilata">
    <dif>
      Iel rilata al duon<tld/>o:
      <ekz>
        li iniciatis kelkajn projektojn de duon<tld/>a skalo
        <klr>(t.e. koncernantajn la tutan orientan a&ubreve; sudan
          a&ubreve; alian duon<tld/>on)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_w;&c_s;&c_f;&c_e;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="ca">
    <trd>semiesf&egrave;ric</trd>,
    <trd>hemisf&egrave;ric</trd>
  </trdgrp>
  <trd lng="de">halbkugelf&ouml;rmig</trd>
  <trd lng="el">&eta;&my;&jota;&sigma;&phi;&alfa;&jota;&rho;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>hemispheric</trd>,
    <trd>hemispherical</trd>
  </trdgrp>
  <trd lng="fr">h&eacute;misph&eacute;rique</trd>
  <trdgrp lng="ja">
    <trd>&#x534A;&#x7403;&#x306E;  <pr>&#x306F;&#x3093;&#x304D;&#x3085;&#x3046;&#x306E;</pr></trd>,
