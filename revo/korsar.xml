<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: korsar.xml,v 1.21 2023/10/18 17:23:25 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>korsar</rad>/o <fnt><bib>FK</bib></fnt>
</kap>
<drv mrk="korsar.0o">
  <kap><tld/>o</kap>
  <snc mrk="korsar.0o.MAR_sxipisto">
    <uzo tip="fak">MAR</uzo>
    <dif>
      &Scirc;ipisto ricevinta permeson a&ubreve; helpon de registaro
      por rabi dum milito komercajn malamikajn &scirc;ipojn:
      <ekz>
        kun sia kutima kura&gcirc;o li kondukas siajn &scirc;ipojn kontra&ubreve; la sova&gcirc;aj <tld/>oj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        niaj soldatoj sin defendas kiel la soldatoj de l' papo: ili &ccirc;iuj surgenui&gcirc;is,
        for&jcirc;etante siajn armilojn kaj petante al la <tld/>o absolvon <klr>[&#x2026;]</klr>
        en la horo de la morto
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>.
      </ekz>
    </dif>
    <!-- tia distingo ne estas aparta por korsaro, kial elstarigi &gcirc;in?....
    <subsnc mrk="korsar.0o.MAR_sxipestro">
      <dif>
        Tia &scirc;ipestro.
      </dif>
      <trdgrp lng="de">
        <trd>Freibeuterkapit&auml;n</trd>,
        <trd>Kaperschiffkapit&auml;n</trd>,
        <trd>Kapit&auml;n eines Korsarschiffs</trd>
      </trdgrp>
      <trd lng="hu">kal&oacute;zkapit&aacute;ny</trd>
      <trd lng="nl">kaperkapitein</trd>
      <trd lng="pt">cors&aacute;rio</trd>
    </subsnc>
    <subsnc mrk="korsar.0o.MAR_sxipano">
      <dif>
        Tia &scirc;ipano.
      </dif>
      <trdgrp lng="de">
        <trd>Freibeuter</trd>,
        <trd>Korsar</trd>
      </trdgrp>
      <trd lng="hu">kal&oacute;z</trd>
      <trdgrp lng="nl">
        <trd>kaper</trd>,
        <trd>piraat</trd>,
        <trd>zeerover</trd>
      </trdgrp>
    </subsnc>
    -->
    <refgrp tip="sin">
      <ref cel="kaper.0isto.MAR">kaperisto</ref>
    </refgrp>
    <ref tip="sub" cel="flibustr.0o">flibustro</ref>
    <refgrp tip="vid">
      <ref cel="pirat.0o.MAR">pirato</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Freibeuter</trd>,
      <trd>Korsar</trd>
    </trdgrp>
    <trd lng="hu">kal&oacute;z</trd>
    <trdgrp lng="nl">
      <trd>kaper</trd>,
      <trd>piraat</trd>,
      <trd>zeerover</trd>
    </trdgrp>
    <trd lng="pl">korsarz</trd>
  </snc>
  <snc mrk="korsar.0o.MAR_sxipo">
    <uzo tip="fak">MAR</uzo>
    <dif>
      <tld lit="K"/><ref tip="super" cel="sxip.0o">&scirc;ipo</ref>:
      <ekz>
        apena&ubreve; ni estis du tagojn sur la maro, forte armita <tld/>o
        komencis &ccirc;asi nin
        <fnt>
          <bib>Hamlet</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Kaperschiff</trd>,
      <trd>Korsarschiff</trd>
    </trdgrp>
    <trd lng="hu">kal&oacute;zhaj&oacute;</trd>
    <trdgrp lng="nl">
      <trd>kaperschip</trd>,
      <trd>kaapvaarder</trd>
    </trdgrp>
    <trd lng="pt">cors&aacute;rio</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_r;&c_s;&c_a;&c_r;</trd>,
    <trd>&c_p;&c_ib;&c_r;&c_a;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>korz&aacute;r</trd>,
    <trd>mo&rcaron;sk&yacute; lupi&ccaron;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>corsair</trd>,
    <trd>privateer</trd>
  </trdgrp>
  <trd lng="fr">corsaire</trd>
  <trdgrp lng="ja">
    <trd>&#x79C1;&#x63A0;&#x8239;&#x8239;&#x9577;  <pr>&#x3057;&#x308A;&#x3083;&#x304F;&#x305B;&#x3093;&#x305B;&#x3093;&#x3061;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x79C1;&#x63A0;&#x8239;&#x4E57;&#x7D44;&#x54E1;  <pr>&#x3057;&#x308A;&#x3083;&#x304F;&#x305B;&#x3093;&#x306E;&#x308A;&#x304F;&#x307F;&#x3044;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>statek <ind>korsarski</ind></trd>,
    <trd>statek <ind>piracki</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_r;&c_s;&c_a;&c_r;</trd>,
    <trd>&c_p;&c_i;&c_r;&c_a;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>korz&aacute;r</trd>,
    <trd>morsk&yacute; lupi&ccaron;</trd>
  </trdgrp>
</drv>
</art>
