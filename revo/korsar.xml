<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>korsar</rad>/o
</kap>
<drv mrk="korsar.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      &Scirc;ipo armita de privatuloj, kun konsento elde registaro, a&ubreve; de
      registaro mem, por rabi dum milito komercajn malamikajn &scirc;ipojn:
      <ekz>
        apena&ubreve; ni estis du tagojn sur la maro, forte armita <tld/>o
        komencis &ccirc;asi nin<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Kaperschiff</trd>,
      <trd>Korsarschiff</trd>
    </trdgrp>
    <trd lng="en">privateer</trd>
    <trd lng="nl">kaperschip</trd>
  </snc>
  <snc>
    <dif>
      Estro de tia &scirc;ipo.
    </dif>
    <trdgrp lng="de">
      <trd>Freibeuterkapit&auml;n</trd>,
      <trd>Kaperschiffkapit&auml;n</trd>,
      <trd>Kapit&auml;n eines Korsarschiffs</trd>
    </trdgrp>
    <trd lng="nl">kapitein<klr tip="ind"> (v.e.kaperschip)</klr></trd>
  </snc>
  <snc>
    <dif>
      Ano de tia &scirc;ipo.
    </dif>
    <refgrp tip="sin">
      <ref cel="pirat.0o">pirato</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="kaper.0i">kaperi</ref>.
    </refgrp>
    <trdgrp lng="en">
      <trd>corsair</trd>,
      <trd>privateer</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Freibeuter</trd>,
      <trd>Korsar</trd>
    </trdgrp>
    <trd lng="nl">zeerover</trd>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_r;&c_s;&c_a;&c_r;</trd>,
    <trd>&c_p;&c_i;&c_r;&c_a;&c_t;</trd>
  </trdgrp>
</drv>
</art>
