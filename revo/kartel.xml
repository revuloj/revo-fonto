<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kartel.xml,v 1.17 2023/09/01 13:58:56 revo Exp $">
<kap>
  <rad>kartel</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kartel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">JUR</uzo>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Interkonsento de organizoj por komuna kunagado:
      <ekz>
        <ind><tld/>o ekonomia</ind>
        <klr>(bazi&gcirc;anta sur kontrakto de produktantoj
        kun la celo forigi la liberan konkuradon kaj influi la prezojn
        la&ubreve;
        komuna reguligo de produktado kaj debito)</klr>;
      </ekz>
      <ekz>
        <ind><tld/>o politika</ind>
        <klr>(interkonsento de politikaj partioj por komuna
        kunagado)</klr>.
        <trd lng="de">Koalition</trd>
        <trd lng="fa">&a_A;&a_y_hamza;&a_t;&a_l;&a_A;&a_f;</trd>
        <trdgrp lng="hu">
          <trd>politikai kartell</trd>,
          <trd>koal&iacute;ci&oacute;</trd>
        </trdgrp>
        <trd lng="nl">coalitie</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konzer.0o">konzerno</ref>,
      <ref cel="trust.0o">trusto</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_t;&c_e;&c_l;&c_mol;</trd>
  <trd lng="cs">kartel</trd>
  <trdgrp lng="de">
    <trd>Kartell</trd>,
    <trd>Ring</trd>
  </trdgrp>
  <trd lng="en">cartel</trd>
  <trd lng="es">c&aacute;rtel</trd>
  <trd lng="fa">&f_k;&a_A;&a_r;&a_t;&a_l;</trd>
  <trd lng="fr">cartel<klr tip="ind"> (entente)</klr></trd>
  <trd lng="hu">kartell</trd>
  <trdgrp lng="ja">
    <trd>&#x30AB;&#x30EB;&#x30C6;&#x30EB;</trd>,
    <trd>&#x9023;&#x5408;  [<ind>&#x308C;&#x3093;&#x3054;&#x3046;</ind>]</trd>,
    <trd>&#x6C7A;&#x95D8;&#x306E;&#x7533;&#x8FBC;&#x72B6;  [<ind>&#x3051;&#x3063;&#x3068;&#x3046;&#x306E;&#x3082;&#x3046;&#x3057;&#x3053;&#x307F;&#x3058;&#x3087;&#x3046;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">kartel</trd>
  <trd lng="pl">kartel</trd>     
  <trd lng="ro">cartel</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_r;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_malmol;&c_je;&c_d;&c_i;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">kartel</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x430;&#x440;&#x442;&#x435;&#x43B;&#x44C;</trd>,
    <trd>&#x431;&#x43B;&#x43E;&#x43A;</trd>,
    <trd>&#x43E;&#x431;&#x2019;&#x454;&#x434;&#x43D;&#x430;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x432;&#x438;&#x43A;&#x43B;&#x438;&#x43A; &#x43D;&#x430; &#x43F;&#x43E;&#x454;&#x434;&#x438;&#x43D;&#x43E;&#x43A;/&#x434;&#x443;&#x435;&#x43B;&#x44C;</trd>
  </trdgrp>
</drv>
</art>
