<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kartel</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kartel.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Interkonsento de organizoj por komuna kunagado. <em><tld/>o
      ekonomia</em> bazi&gcirc;as sur kontrakto de produktantoj en la celo
      forigi la liberan konkuradon kaj influi la prezojn la&ubreve; komuna
      reguligo de produktado kaj debito.
    </dif>
    <refgrp tip="sin">
      <ref cel="konzer.0o">konzerno</ref>,
      <ref cel="trust.0o">trusto</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_t;&c_e;&c_l;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>Kartell</trd>,
    <trd>Ring</trd>
  </trdgrp>
  <trd lng="en">cartel</trd>
  <trd lng="hu">kartell</trd>
  <trd lng="nl">kartel</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_r;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_malmol;&c_je;&c_d;&c_i;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="kartel.0opolitika">
  <kap>
    <tld/>o politika
  </kap>
  <snc>
    <dif>
      Interkonsento de politikaj partioj por komuna kunagado.
    </dif>
  </snc>
  <trd lng="de">Koalition</trd>
  <trdgrp lng="hu">
    <trd>politikai kartell</trd>,
    <trd>koal&iacute;ci&oacute;</trd>
  </trdgrp>
  <trd lng="nl">coalitie</trd>
</drv>
</art>
