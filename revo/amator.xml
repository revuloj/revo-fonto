<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: amator.xml,v 1.27 2022/03/11 17:34:17 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>amator</rad>/o <fnt>Z</fnt> <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="amator.0o">
   <kap><tld/>o</kap>
   <snc mrk="amator.0o.BELA">
     <dif>
       Tiu, kiu amas kaj kolektas belarta&jcirc;ojn a&ubreve; kurioza&jcirc;ojn.
     </dif>
     <trd lng="bg">&c_l;&c_ju;&c_b;&c_i;&c_t;&c_je;&c_l;</trd>
     <trd lng="de">Liebhaber</trd>
     <trd lng="nl">liefhebber</trd>
   </snc>
   <snc mrk="amator.0o.KOMUNE">
     <ref tip="dif" cel="dileta.0o">diletanto</ref>
     <trd lng="bg">&c_a;&c_m;&c_a;&c_t;&c_mol;&c_o;&c_r;</trd>
     <trdgrp lng="de">
       <trd>Nichtfachmann</trd>,
       <trd>Laie</trd>,
       <trd>Amateur</trd>,
       <trd>Amat&ouml;r</trd>
     </trdgrp>
     <trd lng="nl">amateur</trd>
     <trd lng="sk">ochotn&iacute;k</trd>
     <trd lng="sv">amat&ouml;r</trd>
   </snc>
  <trd lng="be">&c_a;&c_m;&c_a;&c_t;&c_a;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>amat&eacute;r</trd>,
    <trd>neodborn&iacute;k</trd>,
    <trd>ochotn&iacute;k</trd>
  </trdgrp>
  <trd lng="en">amateur</trd>
  <trdgrp lng="fr">
    <trd>amateur<klr> (subst.)</klr></trd>,
    <trd>dilettante</trd>
  </trdgrp>
  <trd lng="he">&#x5D7;&#x5D5;&#x5B9;&#x5D1;&#x5B0;&#x5D1;&#x5B8;&#x5DF;</trd>
  <trd lng="ru">&c_l;&c_ju;&c_b;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">amat&eacute;r</trd>
  <trdgrp lng="uk">
    <trd>&#x430;&#x43C;&#x430;&#x442;&#x43E;&#x440;</trd>,
    <trd>&#x43B;&#x44E;&#x431;&#x438;&#x442;&#x435;&#x43B;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="amator.0eca">
   <kap><tld/>eca<fnt><bib>WED</bib></fnt></kap>
   <snc mrk="amator.0eca.KOMUNE">
     <dif>
       Neprofesie aganta,
       <ref tip="sin" cel="dileta.0a">diletanta</ref>.
     </dif>
   </snc>
   <trd lng="be">&c_a;&c_m;&c_a;&c_t;&c_a;&c_r;&c_s;&c_k;&c_ib;</trd>
   <trd lng="bg">&c_a;&c_m;&c_a;&c_t;&c_mol;&c_o;&c_r;&c_s;&c_k;&c_i;</trd>
   <trd lng="en">amateurish</trd>
   <trd lng="fr">amateur<klr> (adj.)</klr></trd>
   <trdgrp lng="de">
     <trd>laienhaft</trd>,
     <trd>dilettantisch</trd>,
     <trd>amateurhaft</trd>,
     <trd>unprofessionell</trd>
   </trdgrp>
   <trd lng="he">&#x5D7;&#x5D5;&#x5B9;&#x5D1;&#x5B0;&#x5D1;&#x5B8;&#x5DF;</trd>
  <trd lng="nl">amateuristisch</trd>
   <trdgrp lng="sk">
     <trd>amat&eacute;rsky</trd>,
     <trd>ochotn&iacute;cky</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_l;&c_ju;&c_b;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_k;&c_i;&c_j;</trd>,
     <trd>&c_d;&c_i;&c_l;&c_je;&c_t;&c_a;&c_n;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>,
     <trd>&c_n;&c_je;&c_p;&c_r;&c_o;&c_f;&c_je;&c_s;&c_s;&c_i;&c_o;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
   </trdgrp>
</drv>

<drv mrk="amator.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="amator.0i.KOMUNE">
    <dif>
       Ne-profesie praktiki iun arto, iun agadon:
       <ekz>
        ne plu oni diros, ke la verda dramarto lamas, <tld/>as
        <fnt>
          <aut>B. Pietrzak</aut>
          <vrk><url ref="http://www2.polskieradio.pl/eo/dokument.aspx?iid=4784">Trarigardo
          de la E-Gazetaro</url></vrk>
          <lok>Pola Radio, 2003-11-05</lok>
        </fnt>;
      </ekz>
      <ekz>
        pri matematiko mi <tld/>as.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">pratiquer en amateur</trd>
</drv>
</art>
