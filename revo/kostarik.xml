<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kostarik</rad>/o
  <ofc>8</ofc>
</kap>

<drv mrk="kostarik.0o">
  <kap>
    <tld lit="K"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="kostarik.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la centra <ref tip="malprt"
      cel="amerik.A0o">Ameriko</ref>
      kun &ccirc;efurbo San-Joseo.
      Teritorio de Respubliko Kostariko.
      ISO-3166 kodo: CR.
      &Gcirc;i limas kun
      <ref tip="vid" cel="nikaragv.0o.GEOG">Nikaragvo</ref>,
      <ref tip="vid" cel="panam.0o.GEOG">Panamo</ref>
      kaj oceanoj:
      <ref tip="vid" cel="atlant.0o">Atlantiko</ref>
      kaj <ref tip="vid" cel="pacifik.0o">Pacifiko</ref>.
    </dif>
    <trdgrp lng="pl">
      <trd>Kostaryka</trd>,
      <trd>Republika <ind>Kostaryki</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

<!--
$Log$
-->
</vortaro>