<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>Kostarik</rad>/o
</kap>

<drv mrk="kostarik.0o">
  <kap>
    <ofc>8</ofc><tld/>o
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/aktoj/aktoj2/8oa.html"> 8a
Oficiala Aldono</url></vrk>, <lok>II.C.1</lok></fnt>
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>
    <fnt><bib>Viki</bib>, <lok><url 
        ref="http://eo.wikipedia.org/wiki/Kostariko">Kostariko</url></lok></fnt>
  </kap>
  <snc mrk="kostarik.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la centra <ref tip="malprt"
      cel="amerik.A0o">Ameriko</ref>
      kun &ccirc;efurbo San-Joseo.
      Teritorio de Respubliko Kostariko.
      ISO-3166 kodo: CR.
      &Gcirc;i limas kun
      <ref tip="vid" cel="nikaragv.N0o.GEOG">Nikaragvo</ref>,
      <ref tip="vid" cel="panam.P0o.GEOG">Panamo</ref>
      kaj oceanoj:
      <ref tip="vid" cel="atlant.A0o">Atlantiko</ref>
      kaj <ref tip="vid" cel="pacifik.P0o">Pacifiko</ref>.
    </dif>
    </snc>
    <trd lng="bg">&c_K;&c_o;&c_s;&c_t;&c_a; &c_R;&c_i;&c_k;&c_a;</trd>
    <trd lng="de">Costa Rica</trd>
    <trd lng="en">Costa Rica</trd>
    <trdgrp lng="fr">
      <trd>Costa Rica</trd>,
      <trd>R&eacute;publique du Costa Rica</trd>
    </trdgrp>    
    <trd lng="id">Kosta Rika</trd>
    <trdgrp lng="pl">
      <trd>Kostaryka</trd>,
      <trd>Republika <ind>Kostaryki</ind></trd>
    </trdgrp>
    <trd lng="pt">Costa Rica</trd>
</drv>

<drv mrk="kostarik.k0ano">
  <kap><tld lit="k"/>ano,
  <var><kap><tld lit="k"/>anino</kap></var></kap>
  <snc>
    <dif>
    Civitano, civitanino de Kostariko
    </dif>
  </snc>
  <trdgrp lng="fr"> 
    <trd>Costaricain</trd>
    <trd>Costaricaine<klr> (subst.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="kostarik.k0a">
  <kap><tld lit="k"/>a</kap>
  <snc>
    <dif>
    Rilata al la &scirc;tato Kostariko
    </dif>
  </snc>
  <trdgrp lng="fr"> 
    <trd>costaricain</trd>
    <trd>costaricaine<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>

</art>

