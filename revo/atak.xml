<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>atak</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="atak.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="atak.0i.batali">
    <dif>
      Komenci batalon kontra&ubreve; malamiko:
      <ekz>
        <tld/>i landon, popolon, urbon;
      </ekz>
      <ekz>
        la lastan el amaso <tld/>as la hundo<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;ipon rompitan &ccirc;iuj ventoj <tld/>as
       <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        uragano <tld/>is la tutan regionon;
      </ekz>
      <ekz>
        <tld/>i iun per paroloj, skriboj;
      </ekz>
      <ekz>
        demando ofte <tld/>ita sed neniam solvita.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="siegx.0i">sie&gcirc;i</ref>.
    </refgrp>
    <trd lng="nl">aanvallen</trd>
    <trd lng="sk">za&uacute;to&ccaron;i&tcaron;</trd>
    <trd lng="tp">utala</trd>
  </snc>
  <snc mrk="atak.0i.ekkapti">
    <dif>
      Ekkapti, trafi, frapi (pri sentoj kaj malsanoj):
      <ekz>
        la febro, tremo<fnt>Z</fnt>,
        suferego<fnt>Z</fnt> lin <tld/>is;
      </ekz>
      <ekz>
        sopiro al la hejmo lin <tld/>is<fnt>Z</fnt>;
      </ekz>
      <ekz>
        teruro ilin <tld/>is
        <fnt><bib>MT</bib>, <lok>&Psa; 48:6</lok></fnt>;
      </ekz>
      <ekz>
        la deziro <tld/>is min, ke...;
      </ekz>
      <ekz>
        novaj, &gcirc;is nun nekonataj pensoj <tld/>is la kapon de
        la juna virino<fnt>Z</fnt>;
      </ekz>
      <ekz>
        furioza pu&scirc;o de pasio
        <tld/>is &scirc;in<fnt>Z</fnt>;
      </ekz>
      <ekz>
        krizo <tld/>is la industrion.
      </ekz>
    </dif>
    <trd lng="nl">overvallen</trd>
  </snc>
  <snc mrk="atak.0i.ekdifekti">
    <dif>
      Ekdifekti, mordi:
      <ekz>
        rusto <tld/>as feron.
      </ekz>
    </dif>
    <trd lng="nl">inwerken op</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_t;&c_a;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>angreifen</trd>,
    <trd>attackieren</trd>
  </trdgrp>
  <trd lng="en">attack</trd>
  <trd lng="es">atacar</trd>
  <trd lng="fr">attaquer</trd>
  <trdgrp lng="he">
    <trd>&lamed;<ind>&tav;&qof;&vav;&fpe;</ind></trd>,
    <trd>&lamed;<ind>&he;&tav;&qof;&yod;&fpe;</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>t&aacute;mad</trd>,
    <trd>megt&aacute;mad</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>attaccare</trd>,
    <trd>assalire</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_t;&c_a;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_a;&c_d;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">napadn&uacute;&tcaron;</trd>
</drv>

<drv mrk="atak.0o">
  <kap><tld/>o</kap>
  <snc mrk="atak.0o.batalo">
    <dif>
      Ago, kiu komencas batalon:
      <ekz>
        <tld/>o de rabistoj.
      </ekz>
    </dif>
  </snc>
  <snc mrk="atak.0o.ekkapto">
    <dif>
      Subita apero de emocio, afekcio, sento, malsano:
      <ekz>
        <tld/>o de &gcirc;ojo, rido, pasio, kolero.
      </ekz>
    </dif>
  </snc>
  <snc mrk="atak.0o.ekdifekto">
    <dif>
      Ekdifektado de substanco.
    </dif>
  </snc>
 <trd lng="fr">attaque</trd> 
 <trdgrp lng="it">
    <trd>attacco</trd>,
    <trd>assaltoe</trd>
  </trdgrp>
</drv>

<drv mrk="atak.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <dif>
      Batalema, spitema:
      <ekz>
        <tld/>ema humoro;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>emo <klr>(fortego, torenteco)</klr> de la
        vento<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_ja;&c_w;&c_n;&c_ib;&c_ch;&c_y;</trd>
  <trd lng="de">angriffslustig</trd>
  <trdgrp lng="en">
    <trd>belligerent</trd>,
    <trd>bellicose</trd>,
    <trd>pugnacious</trd>,
    <trd>aggressive</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>agressif</trd>,
    <trd>querelleur</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&tav;&vav;&qof;&pe;&nun;&yod;</trd>,
    <trd>&tav;&vav;&qof;&fpe;</trd>,
    <trd>&lamed;&vav;&het;&mem;&nun;&yod;</trd>,
    <trd>&tav;&qof;&yod;&fpe;</trd>
  </trdgrp>
  <trd lng="hu">t&aacute;mad&oacute;kedv&udblac;</trd>
  <trdgrp lng="it">
    <trd>aggressivo</trd>,
    <trd>bellicoso</trd>
  </trdgrp>
  <trd lng="nl">aggressief</trd>
  <trd lng="ru">&c_v;&c_o;&c_i;&c_n;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">&uacute;to&ccaron;n&yacute;</trd>
</drv>

<drv mrk="atak.surpriz0o">
  <kap>surpriz<tld/>o</kap>
  <snc mrk="atak.surpriz0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Subita, neatendita <tld/>o:
      <ekz>
        separatismaj grupoj ofte surpri<tld/>as
        en hindian teritorion
        <fnt>
          <aut>J. Holslag</aut>,
          <vrk><url
          ref="http://www.vrede.be/listo-overzicht/1707-hindia-unio-terorplena-tato-n-ro-395--jan-feb-2009-">
          Hindia Unio, terorplena &scirc;tato</url></vrk>,
          <lok>Vrede, 2009-01 &gcirc;is 02 (395)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">raid</trd>
  <trd lng="it">blitz</trd>
</drv>
</art>
