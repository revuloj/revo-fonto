<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: atak.xml,v 1.41 2023/10/28 08:33:09 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>atak</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="atak.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="atak.0i.batali">
    <dif>
      Komenci batalon kontra&ubreve; malamiko:
      <ekz>
        du viroj estis <tld/>itaj de duonroto da soldatoj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        Croto <tld/>is min, por min murdi kaj rabi
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        rabisto <tld/>is <klr>[&#x2026;]</klr> vian rikolton
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 48:32</lok>
        </fnt>;
      </ekz>
      <ekz>
        kure alvenis la gardistoj, al kiuj &scirc;i mensogis, ke mi <tld/>is &scirc;in
        <fnt>
          <bib>VaK</bib>
          <lok>Dekkvina &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiun landon <klr>[&#x2026;]</klr> moskova caro &gcirc;in <tld/>is
        <fnt>
          <bib>VivZam</bib>
          <lok>Vivo de Zamenhof</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>is nin <klr>[&#x2026;]</klr> uragana vento
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 27:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la malamikoj de la homaranismo <tld/>as la tutan esencon de la afero
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Kion Zamenhof ne povis diri en &Gcirc;enevo.</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;ipon rompitan &ccirc;iuj ventoj <tld/>as
       <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>la lastan el amaso <tld/>as la hundo</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="de">den Letzten bei&szlig;en die Hunde</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="agres.0i">agresi</ref>,
      <ref cel="milit.al0i">almiliti</ref>,
      <ref cel="invad.0i">invadi</ref>,
      <ref cel="siegx.0i">sie&gcirc;i</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>angreifen</trd>,
      <trd>attackieren</trd>
    </trdgrp>
    <trd lng="nl">aanvallen</trd>
    <trd lng="pt">atacar</trd>
    <trd lng="sk">za&uacute;to&ccaron;i&tcaron;</trd>
    <trd lng="tok">utala</trd>
    <trdgrp lng="zh">
    <trd>&#x62A8; <pr>p&#x113;ng</pr></trd>,
    <trd>&#x653B;&#x6253; <pr>g&#x14D;ngd&#x1CE;</pr></trd>,
    <trd>&#x62A8;&#x51FB; <pr>p&#x113;ngj&#x12B;</pr></trd>,
    <trd>&#x62A8;&#x64CA; <pr>p&#x113;ngj&#x12B;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="atak.0i.ekkapti">
    <dif>
      <ref tip="sin" cel="kapt.ek0i">Ekkapti</ref>, <ref tip="sin" cel="traf.0i.frapi">trafi</ref>, 
      <ref tip="sin" cel="frap.0i.tusxegi">frapi</ref> (pri sentoj kaj malsanoj):
      <ekz>
        teruro ilin <tld/>is
        <fnt><bib>MT</bib>, <lok>&Psa; 48:6</lok></fnt>;
      </ekz>
      <ekz>
        suferego <tld/>is lin, doloro kiel &ccirc;e naskantino;
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 50:43</lok>
        </fnt>;
      </ekz>
      <ekz>
        sopiro al la hejmo ilin <tld/>is
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        tremo ilin <tld/>os
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 30:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        frenezo febroforma lin <tld/>as
        <fnt>
          <bib>Ifigenio</bib>
          <lok>Ifigenio en Ta&ubreve;rido</lok>
        </fnt>;
      </ekz>
      <ekz>
        kapturni&gcirc;o <tld/>is min super la beleco kaj okulfrapa lukso
        <fnt>
          <bib>SkandalJozef</bib>
          <lok>Plendoj de Jozefo arestita</lok>
        </fnt>;
      </ekz>
      <ekz>
        malespero lin <tld/>as
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        novaj, &gcirc;is nun nekonataj pensoj <tld/>is la kapon de
        la juna virino
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        frosto lin <tld/>is
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="afekci.0i">afekcii</ref>,
      <ref cel="tusx.0egi">tu&scirc;egi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>angreifen<klr> (Gesundheit)</klr></trd>,
      <trd>einen <ind>erwischen</ind></trd>,
      <trd>zu <ind>schaffen</ind> machen</trd>
      <trd>&uuml;berkommen <klr>(angreifen)</klr></trd>
    </trdgrp>
    <trd lng="nl">overvallen</trd>
  </snc>
  <snc mrk="atak.0i.ekdifekti">
    <dif>
      Ek<ref tip="super" cel="difekt.0i">difekti</ref>, 
      <ref tip="sin" cel="mord.0i.ataki">mordi</ref>:
      <ekz>
        rusto <tld/>as feron.
      </ekz>
    </dif>
    <trd lng="de">angreifen</trd>
    <trd lng="nl">inwerken op</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_t;&c_a;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>obo&rcaron;it se (na n&ecaron;koho)</trd>,
    <trd>projevit nesouhlas</trd>,
    <trd>vystoupit <klr>(proti n&ecaron;komu)</klr></trd>
  </trdgrp>
  <trd lng="en">attack</trd>
  <trd lng="es">atacar</trd>
  <trd lng="fr">attaquer</trd>
  <trdgrp lng="he">
    <trd>&lamed;<ind>&tav;&qof;&vav;&fpe;</ind></trd>,
    <trd>&lamed;<ind>&he;&tav;&qof;&yod;&fpe;</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>t&aacute;mad</trd>,
    <trd>megt&aacute;mad</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>attaccare</trd>,
    <trd>assalire</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x653B;&#x6483;&#x3059;&#x308B;  <pr>&#x3053;&#x3046;&#x3052;&#x304D;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x8972;&#x3046;  <pr>&#x304A;&#x305D;&#x3046;</pr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_t;&c_a;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_a;&c_d;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>napadn&uacute;&tcaron;</trd>,
    <trd>vyst&uacute;pi&tcaron; proti niekomu</trd>
  </trdgrp>
  <trd lng="uk">&c_d;&c_l;&c_ja; &c_p;&c_o;&c_z;&c_n;&c_a;&c_ch;&c_je;&c_n;&c_n;&c_ja; &c_k;&c_i;&c_s;&c_l;&c_i;&c_h;
  &c_s;&c_o;&c_l;&c_je;&c_j;</trd>
</drv>

<drv mrk="atak.0o">
  <kap><tld/>o</kap>
  <snc mrk="atak.0o.batalo">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Ago, kiu komencas batalon:
      <ekz>
        <tld/>o de rabistoj;
      </ekz>
      <ekz>
        gardi la propran armeon kontra&ubreve; <tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        la trumpetoj ekludis al la <tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi petas vin, al la tablo! <klr>[&#x2026;]</klr>
        ili proksimi&gcirc;is, eltiris la tran&ccirc;ilojn kaj brave komencis la <tld/>on
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tablo, la orazeno kaj bastono el sako</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la unuaj fortegaj <tld/>oj de la filozofoj kontra&ubreve; la jezuitoj
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        hako post hako estas la plej efika <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ofensi.0o">ofensivo</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_a;&c_t;&c_a;&c_k;&c_a;</trd>,
      <trd>&c_n;&c_a;&c_p;&c_a;&c_d;</trd>
    </trdgrp>
    <trd lng="de">Angriff</trd>
    <trdgrp lng="zh">
    <trd>&#x6B7C;&#x51FB; <pr>ji&#x101;nj&#x12B;</pr></trd>,
    <trd>&#x6BB2;&#x64CA; <pr>ji&#x101;nj&#x12B;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="atak.0o.ekkapto">
    <dif>
      Subita apero de emocio, afekcio, sento, malsano:
      <ekz>
        <tld/>o de rido, pasio;
      </ekz>
      <ekz>
        en <tld/>o de &gcirc;ojo kaj konfuzo, kun <klr>[&#x2026;]</klr> amo kaj kompato, li rigardis
        la elturmentitan viza&gcirc;on
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Tutmozis</nom> timigita atendis, &gcirc;is la <tld/>o de furiozo forlasos la princon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        minacas <tld/>o de krupo
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        helpu al mi min levi! &gcirc;i estas nur <tld/>o de senkonscieco
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frap.0o">frapo</ref>,
      <ref cel="kriz.0o">krizo</ref>
    </refgrp>
    <trd lng="be">&c_p;&c_r;&c_y;&c_s;&c_t;&c_u;&c_p;</trd>
    <trd lng="de">Anfall<klr> (Herz-, Wut-, ...von Heiterkeit)</klr></trd>
    <trdgrp lng="zh">
    <trd>&#x77AC;&#x9593;&#x885D;&#x52D5; <pr>sh&#xF9;nji&#x101;nch&#x14D;ngd&#xF2;ng</pr></trd>,
    <trd>&#x77AC;&#x95F4;&#x51B2;&#x52A8; <pr>sh&#xF9;nji&#x101;nch&#x14D;ngd&#xF2;ng</pr></trd>,
    <trd>&#x72C2;&#x6012; <pr>ku&#xE1;ngn&#xF9;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="atak.0o.ekdifekto">
    <dif>
      Ekdifektado de substanco.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>atak</trd>,
    <trd>ataka</trd>,
    <trd>napaden&iacute;</trd>,
    <trd>n&aacute;jezd</trd>,
    <trd>p&rcaron;epaden&iacute;</trd>,
    <trd>&uacute;tok</trd>
  </trdgrp>
  <trd lng="fr">attaque</trd>
  <trd lng="he">&he;&tav;&qof;&pe;&he;</trd>
  <trdgrp lng="it">
    <trd>attacco</trd>,
    <trd>assaltoe</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x653B;&#x6483;  <pr>&#x3053;&#x3046;&#x3052;&#x304D;</pr></trd>,
    <trd>&#x767A;&#x4F5C;  <pr>&#x307B;&#x3063;&#x3055;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>napadnutie</trd>,
    <trd>&uacute;tok</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&c_t;&c_a;&c_k;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_a;&c_d;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_s;&c_t;&c_u;&c_p;</trd>,
    <trd>&c_v;&c_i;&c_p;&c_a;&c_d;</trd>
  </trdgrp>
</drv>

<drv mrk="atak.0anto">
  <kap><ofc>*</ofc><tld/>anto</kap>
  <snc mrk="atak.0anto.batalo">
    <uzo tip="fak">SPO</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Homo kiu <tld/>as, komencas batalon, sportludon ktp:
      <ekz>
        Kvinope ili sin &jcirc;etis sur min, sed mi venkis &ccirc;iujn kvin
        <tld/>antojn
        <fnt><bib>F</bib>, <lok>&FE; 14</lok></fnt>;
      </ekz>
      <ekz>
        Vi <klr>[&#x2026;]</klr> helpas la fidantojn kontra&ubreve; la <tld/>antoj
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 17:7</lok>
        </fnt>;
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x653B;&#x51FB;&#x8005; <pr>g&#x14D;ngj&#x12B;zh&#x11B;</pr></trd>,
    <trd>&#x653B;&#x64CA;&#x8005; <pr>g&#x14D;ngj&#x12B;zh&#x11B;</pr></trd>,
    <trd>&#x8FDB;&#x653B;&#x8005; <pr>j&#xEC;ng&#x14D;ngzh&#x11B;</pr></trd>,
    <trd>&#x9032;&#x653B;&#x8005; <pr>j&#xEC;ng&#x14D;ngzh&#x11B;</pr></trd>,
    <trd>&#x4FB5;&#x7565;&#x8005; <pr>q&#x12B;nl&#xFC;&#xE8;zh&#x11B;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="atak.0anto.FIG">
    <dif>
      Verva kritikanto:
      <ekz>
        de iliaj senka&ubreve;zaj <tld/>oj Esperanto ne pereos, kiel &gcirc;i ne pereis &gcirc;is nun,
        malgra&ubreve; multaj miloj da <tld/>antoj
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Kion Zamenhof ne povis diri en &Gcirc;enevo.</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x5192;&#x72AF;&#x8005; <pr>m&#xE0;of&#xE0;nzh&#x11B;</pr></trd>
</snc>
  <trd lng="de">Angreifer</trd>
  <trd lng="fr">attaquant</trd>
  <trd lng="he">&tav;&vav;&#x5B9;&qof;&#x5B5;&fpe;</trd>
  <trd lng="ja">&#x653B;&#x6483;&#x8005;  <pr>&#x3053;&#x3046;&#x3052;&#x304D;&#x3057;&#x3083;</pr></trd>
  <trdgrp lng="uk">
    <trd>&c_n;&c_a;&c_p;&c_a;&c_d;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_a;&c_d;&c_a;&c_ju;&c_ch;&c_i;&c_j;&nbsp;(&c_ib;&c_m;.)</trd>
  </trdgrp>
</drv>

<drv mrk="atak.0antino">
  <kap><tld/>antino</kap>
  <snc mrk="atak.0antino.batalo">
    <uzo tip="fak">SPO</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Virina <tld/>anto.
    </dif>
    <trdgrp lng="zh">
    <trd>&#x88AD;&#x51FB;&#x8005; <pr>x&#xED;j&#x12B;zh&#x11B;</pr></trd>,
    <trd>&#x8972;&#x64CA;&#x8005; <pr>x&#xED;j&#x12B;zh&#x11B;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="de">Angreiferin</trd>
  <trd lng="fr">attaquante</trd>
</drv>

<drv mrk="atak.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="atak.0ema.KOMUNE">
    <dif>
      <ref cel="batal.0ema">Batalema</ref>, <ref cel="spit.0ema">spitema</ref>:
      <ekz>
        <tld/>ema kiel leono
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi vidas la&ubreve; lia nazo, ke li estas forte <tld/>ema
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        doktoro <nom>Rees</nom> ofte ofendi&gcirc;is pro lia <tld/>ema malkredemo
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Franjo, spirito <tld/>ema, ekparolis vive per akra vo&ccirc;o
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>emo de la ventego kaj la terura varmego
        kreskis senhalte
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 19a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x6C14;&#x52BF;&#x6C79;&#x6C79; <pr>q&#xEC;sh&#xEC;xi&#x14D;ngxi&#x14D;ng</pr></trd>,
    <trd>&#x6C23;&#x52E2;&#x6D36;&#x6D36; <pr>q&#xEC;sh&#xEC;xi&#x14D;ngxi&#x14D;ng</pr></trd>,
    <trd>&#x5FEE; <pr>zh&#xEC;</pr></trd>,
    <trd>&#x597D;&#x6597; <pr>h&#x1CE;od&#xF2;u</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_v;&c_a;&c_ja;&c_w;&c_n;&c_ib;&c_ch;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>agresivn&iacute;</trd>,
    <trd>&uacute;to&ccaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="de">angriffslustig</trd>
  <trdgrp lng="en">
    <trd>belligerent</trd>,
    <trd>bellicose</trd>,
    <trd>pugnacious</trd>,
    <trd>aggressive</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>agressif</trd>,
    <trd>querelleur</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&tav;&vav;&qof;&pe;&nun;&yod;</trd>,
    <trd>&tav;&vav;&qof;&fpe;</trd>,
    <trd>&lamed;&vav;&het;&mem;&nun;&yod;</trd>,
    <trd>&tav;&qof;&yod;&fpe;</trd>
  </trdgrp>
  <trd lng="hu">t&aacute;mad&oacute;kedv&udblac;</trd>
  <trdgrp lng="it">
    <trd>aggressivo</trd>,
    <trd>bellicoso</trd>
