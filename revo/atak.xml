<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: atak.xml,v 1.47 2025/09/20 08:34:55 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>atak</rad>/i <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="atak.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="atak.0i.batali">
    <dif>
      Komenci batalon kontra&ubreve; malamiko:
      <ekz>
        du viroj estis <tld/>itaj de duonroto da soldatoj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Croto</nom> <tld/>is min, por min murdi kaj rabi
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        rabisto <tld/>is <klr>[&#8230;]</klr> vian rikolton
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 48:32</lok>
        </fnt>;
      </ekz>
      <ekz>
        kure alvenis la gardistoj, al kiuj &scirc;i mensogis, ke mi <tld/>is &scirc;in
        <fnt>
          <bib>VaK</bib>
          <lok>Dekkvina &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiun landon <klr>[&#8230;]</klr> moskova caro &gcirc;in <tld/>is
        <fnt>
          <bib>VivZam</bib>
          <lok>Vivo de Zamenhof</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>is nin <klr>[&#8230;]</klr> uragana vento
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 27:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la malamikoj de la homaranismo <tld/>as la tutan esencon de la afero
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Kion Zamenhof ne povis diri en &Gcirc;enevo.</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;ipon rompitan &ccirc;iuj ventoj <tld/>as
       <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>la lastan el amaso <tld/>as la hundo</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="de">den Letzten bei&szlig;en die Hunde</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="agres.0i">agresi</ref>,
      <ref cel="milit.al0i">almiliti</ref>,
      <ref cel="invad.0i">invadi</ref>,
      <ref cel="siegx.0i">sie&gcirc;i</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>angreifen</trd>,
      <trd>attackieren</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd><baz>gerebek</baz>menggerebek</trd>,
      <trd><baz>serang</baz>menyerang</trd>,
      <trd><baz>serbu</baz>menyerbu</trd>,
      <trd><baz>sergap</baz>menyergap</trd>
    </trdgrp>
    <trd lng="nl">aanvallen</trd>
    <trd lng="pt">atacar</trd>
    <trd lng="sk">za&uacute;to&ccaron;i&tcaron;</trd>
    <trd lng="tok">utala</trd>
    <trdgrp lng="zh">
    <trd>&#25256; <pr>p&emacron;ng</pr></trd>,
    <trd>&#25915;&#25171; <pr>g&omacron;ngd&#462;</pr></trd>,
    <trd>&#25256;&#20987; <pr>p&emacron;ngj&imacron;</pr></trd>,
    <trd>&#25256;&#25802; <pr>p&emacron;ngj&imacron;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="atak.0i.ekkapti">
    <dif>
      <ref tip="sin" cel="kapt.ek0i">Ekkapti</ref>, <ref tip="sin" cel="traf.0i.frapi">trafi</ref>,
      <ref tip="sin" cel="frap.0i.tusxegi">frapi</ref> (pri sentoj kaj malsanoj):
      <ekz>
        teruro ilin <tld/>is
        <fnt><bib>MT</bib>, <lok>&Psa; 48:6</lok></fnt>;
      </ekz>
      <ekz>
        suferego <tld/>is lin, doloro kiel &ccirc;e naskantino;
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 50:43</lok>
        </fnt>;
      </ekz>
      <ekz>
        sopiro al la hejmo ilin <tld/>is
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        tremo ilin <tld/>os
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 30:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        frenezo febroforma lin <tld/>as
        <fnt>
          <bib>Ifigenio</bib>
          <lok>Ifigenio en Ta&ubreve;rido</lok>
        </fnt>;
      </ekz>
      <ekz>
        kapturni&gcirc;o <tld/>is min super la beleco kaj okulfrapa lukso
        <fnt>
          <bib>SkandalJozef</bib>
          <lok>Plendoj de Jozefo arestita</lok>
        </fnt>;
      </ekz>
      <ekz>
        malespero lin <tld/>as
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        novaj, &gcirc;is nun nekonataj pensoj <tld/>is la kapon de
        la juna virino
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        frosto lin <tld/>is
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="afekci.0i">afekcii</ref>,
      <ref cel="tusx.0egi">tu&scirc;egi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>angreifen<klr> (Gesundheit)</klr></trd>,
      <trd>einen <ind>erwischen</ind></trd>,
      <trd>zu <ind>schaffen</ind> machen</trd>
      <trd>&uuml;berkommen <klr>(angreifen)</klr></trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd><baz>landa</baz>melanda</trd>,
      <trd><baz>timpa</baz>menimpa</trd>
    </trdgrp>
    <trd lng="nl">overvallen</trd>
  </snc>
  <snc mrk="atak.0i.ekdifekti">
    <dif>
      Ek<ref tip="super" cel="difekt.0i">difekti</ref>,
      <ref tip="sin" cel="mord.0i.ataki">mordi</ref>:
      <ekz>
        rusto <tld/>as feron.
      </ekz>
    </dif>
    <trd lng="de">angreifen</trd>
    <trd lng="nl">inwerken op</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_t;&c_a;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>obo&rcaron;it se (na n&ecaron;koho)</trd>,
    <trd>projevit nesouhlas</trd>,
    <trd>vystoupit <klr>(proti n&ecaron;komu)</klr></trd>
  </trdgrp>
  <trd lng="en">attack</trd>
  <trd lng="es">atacar</trd>
  <trd lng="fr">attaquer</trd>
  <trdgrp lng="he">
    <trd>&lamed;<ind>&tav;&qof;&vav;&fpe;</ind></trd>,
    <trd>&lamed;<ind>&he;&tav;&qof;&yod;&fpe;</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>t&aacute;mad</trd>,
    <trd>megt&aacute;mad</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>attaccare</trd>,
    <trd>assalire</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#25915;&#25731;&#12377;&#12427;  <pr>&#12371;&#12358;&#12370;&#12365;&#12377;&#12427;</pr></trd>,
    <trd>&#35186;&#12358;  <pr>&#12362;&#12381;&#12358;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>napada&cacute;</trd>,
    <trd>atakowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>ataca</trd>,
    <trd>asalta</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_t;&c_a;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_a;&c_d;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>napadn&uacute;&tcaron;</trd>,
    <trd>vyst&uacute;pi&tcaron; proti niekomu</trd>
  </trdgrp>
  <trd lng="uk">&c_d;&c_l;&c_ja; &c_p;&c_o;&c_z;&c_n;&c_a;&c_ch;&c_je;&c_n;&c_n;&c_ja; &c_k;&c_i;&c_s;&c_l;&c_i;&c_h;
  &c_s;&c_o;&c_l;&c_je;&c_j;</trd>
</drv>

<drv mrk="atak.0o">
  <kap><tld/>o</kap>
  <snc mrk="atak.0o.batalo">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Ago, kiu komencas batalon:
      <ekz>
        <tld/>o de rabistoj;
      </ekz>
      <ekz>
        gardi la propran armeon kontra&ubreve; <tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        la trumpetoj ekludis al la <tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi petas vin, al la tablo! <klr>[&#8230;]</klr>
        ili proksimi&gcirc;is, eltiris la tran&ccirc;ilojn kaj brave komencis la <tld/>on
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tablo, la orazeno kaj bastono el sako</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la unuaj fortegaj <tld/>oj de la filozofoj kontra&ubreve; la jezuitoj
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        hako post hako estas la plej efika <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ofensi.0o">ofensivo</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_a;&c_t;&c_a;&c_k;&c_a;</trd>,
      <trd>&c_n;&c_a;&c_p;&c_a;&c_d;</trd>
    </trdgrp>
    <trd lng="de">Angriff</trd>
    <trdgrp lng="id">
      <trd>agresi</trd>,
      <trd><baz>gerebek</baz>penggerebekan</trd>,
      <trd><baz>serang</baz>serangan</trd>,
      <trd><baz>serang</baz>penyerangan</trd>,
      <trd><baz>serbu</baz>serbuan</trd>,
      <trd><baz>serbu</baz>penyerbuan</trd>,
      <trd><baz>sergap</baz>sergapan</trd>,
      <trd><baz>sergap</baz>penyergapan</trd>,
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#27516;&#20987; <pr>ji&amacron;nj&imacron;</pr></trd>,
    <trd>&#27570;&#25802; <pr>ji&amacron;nj&imacron;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="atak.0o.ekkapto">
    <dif>
      Subita apero de emocio, afekcio, sento, malsano:
      <ekz>
        <tld/>o de rido, pasio;
      </ekz>
      <ekz>
        en <tld/>o de &gcirc;ojo kaj konfuzo, kun <klr>[&#8230;]</klr> amo kaj kompato, li rigardis
        la elturmentitan viza&gcirc;on
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Tutmozis</nom> timigita atendis, &gcirc;is la <tld/>o de furiozo forlasos la princon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        minacas <tld/>o de krupo
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        helpu al mi min levi! &gcirc;i estas nur <tld/>o de senkonscieco
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frap.0o">frapo</ref>,
      <ref cel="kriz.0o">krizo</ref>
    </refgrp>
    <trd lng="be">&c_p;&c_r;&c_y;&c_s;&c_t;&c_u;&c_p;</trd>
    <trd lng="de">Anfall<klr> (Herz-, Wut-, ...von Heiterkeit)</klr></trd>
    <trdgrp lng="zh">
    <trd>&#30636;&#38291;&#34909;&#21205; <pr>sh&ugrave;nji&amacron;nch&omacron;ngd&ograve;ng</pr></trd>,
    <trd>&#30636;&#38388;&#20914;&#21160; <pr>sh&ugrave;nji&amacron;nch&omacron;ngd&ograve;ng</pr></trd>,
    <trd>&#29378;&#24594; <pr>ku&aacute;ngn&ugrave;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="atak.0o.ekdifekto">
    <dif>
      Ekdifektado de substanco.
    </dif>
  <trd lng="be">&c_k;&c_a;&c_r;&c_a;&c_z;&c_ib;&c_j;&c_n;&c_a;&c_je; &c_p;&c_a;&c_sh;&c_k;&c_o;&c_d;&c_zh;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>atak</trd>,
    <trd>ataka</trd>,
    <trd>napaden&iacute;</trd>,
    <trd>n&aacute;jezd</trd>,
    <trd>p&rcaron;epaden&iacute;</trd>,
    <trd>&uacute;tok</trd>
  </trdgrp>
  <trd lng="fr">attaque</trd>
  <trd lng="he">&he;&tav;&qof;&pe;&he;</trd>
  <trdgrp lng="it">
    <trd>attacco</trd>,
    <trd>assaltoe</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#25915;&#25731;  <pr>&#12371;&#12358;&#12370;&#12365;</pr></trd>,
    <trd>&#30330;&#20316;  <pr>&#12411;&#12387;&#12373;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>atak</trd>,
    <trd>ofensywa</trd>,
    <trd>napad</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>atac</trd>,
    <trd>acces</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>napadnutie</trd>,
    <trd>&uacute;tok</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&c_t;&c_a;&c_k;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_a;&c_d;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_s;&c_t;&c_u;&c_p;</trd>,
    <trd>&c_v;&c_i;&c_p;&c_a;&c_d;</trd>
  </trdgrp>
</drv>

<drv mrk="atak.0anto">
  <kap><ofc>*</ofc><tld/>anto</kap>
  <snc mrk="atak.0anto.batalo">
    <uzo tip="fak">SPO</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Homo kiu <tld/>as, komencas batalon, sportludon ktp:
      <ekz>
        Kvinope ili sin &jcirc;etis sur min, sed mi venkis &ccirc;iujn kvin
        <tld/>antojn
        <fnt><bib>F</bib>, <lok>&FE; 14</lok></fnt>;
      </ekz>
      <ekz>
        Vi <klr>[&#8230;]</klr> helpas la fidantojn kontra&ubreve; la <tld/>antoj
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 17:7</lok>
        </fnt>;
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#25915;&#20987;&#32773; <pr>g&omacron;ngj&imacron;zh&ecaron;</pr></trd>,
    <trd>&#25915;&#25802;&#32773; <pr>g&omacron;ngj&imacron;zh&ecaron;</pr></trd>,
    <trd>&#36827;&#25915;&#32773; <pr>j&igrave;ng&omacron;ngzh&ecaron;</pr></trd>,
    <trd>&#36914;&#25915;&#32773; <pr>j&igrave;ng&omacron;ngzh&ecaron;</pr></trd>,
    <trd>&#20405;&#30053;&#32773; <pr>q&imacron;nl&uuml;&egrave;zh&ecaron;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="atak.0anto.FIG">
    <dif>
      Verva kritikanto:
      <ekz>
        de iliaj senka&ubreve;zaj <tld/>oj Esperanto ne pereos, kiel &gcirc;i ne pereis &gcirc;is nun,
        malgra&ubreve; multaj miloj da <tld/>antoj
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Kion Zamenhof ne povis diri en &Gcirc;enevo.</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#20882;&#29359;&#32773; <pr>m&agrave;of&agrave;nzh&ecaron;</pr></trd>
</snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_p;&c_a;&c_d;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_a;&c_d;&c_a;&c_ju;&c_ch;&c_y;</trd>
  </trdgrp>
  <trd lng="de">Angreifer</trd>
  <trd lng="fr">attaquant</trd>
  <trd lng="he">&tav;&vav;&#1465;&qof;&#1461;&fpe;</trd>
  <trdgrp lng="id">
    <trd><baz>gerebek</baz>penggerebek</trd>,
    <trd><baz>serang</baz>penyerang</trd>,
    <trd><baz>serbu</baz>penyerbu</trd>,
    <trd><baz>sergap</baz>penyergap</trd>
  </trdgrp>
  <trd lng="ja">&#25915;&#25731;&#32773;  <pr>&#12371;&#12358;&#12370;&#12365;&#12375;&#12419;</pr></trd>
  <trdgrp lng="pl">
    <trd>atakuj&aogonek;cy</trd>,
    <trd>napastnik</trd>,
    <trd>naje&zacute;d&zacute;ca</trd>,
    <trd>agresor</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_a;&c_p;&c_a;&c_d;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_a;&c_d;&c_a;&c_ju;&c_ch;&c_i;&c_j;&nbsp;(&c_ib;&c_m;.)</trd>
  </trdgrp>
</drv>

<drv mrk="atak.0antino">
  <kap><tld/>antino</kap>
  <snc mrk="atak.0antino.batalo">
    <uzo tip="fak">SPO</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Virina <tld/>anto.
    </dif>
    <trdgrp lng="zh">
    <trd>&#34989;&#20987;&#32773; <pr>x&iacute;j&imacron;zh&ecaron;</pr></trd>,
    <trd>&#35186;&#25802;&#32773; <pr>x&iacute;j&imacron;zh&ecaron;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_n;&c_a;&c_p;&c_a;&c_d;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="de">Angreiferin</trd>
  <trd lng="fr">attaquante</trd>
  <trdgrp lng="id">
    <trd><baz>gerebek</baz>penggerebek<klr tip="amb"> (perempuan)</klr></trd>,
    <trd><baz>serang</baz>penyerang<klr tip="amb"> (perempuan)</klr></trd>,
    <trd><baz>serbu</baz>penyerbu<klr tip="amb"> (perempuan)</klr></trd>,
    <trd><baz>sergap</baz>penyergap<klr tip="amb"> (perempuan)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>agresorka</trd>,
    <trd>napastniczka</trd>
  </trdgrp>
</drv>

<drv mrk="atak.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="atak.0ema.KOMUNE">
    <dif>
