</drv>

<drv mrk="atak.vort0i">
  <kap>vort<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="atak.vort0i.KOMUNE">
    <dif>
      <tld lit="A"/>i per malfavoraj diroj,
      tre malla&ubreve;di, forte <ref tip="vid" cel="kritik.0i.KOMUNE">kritiki</ref>:
      <ekz>
        &leftquot;vi estas stultulo kaj instruojn miajn vi ne
        memoras,&rightquot; vort<tld/>is lin la maljuna sinjoro
        <fnt>
          <aut>B. N&ecaron;mcov&aacute;, trad. J. Patera</aut>,
          <vrk><url ref="http://www.literatura.bucek.name/nemcova/kaca.html">
          Diablo kaj Ka&ccirc;a</url></vrk>,
          <lok>[2009?]</lok>
        </fnt>;
      </ekz>
      <ekz>
        la inundo de etaj vort<tld/>oj sen inteligenta argumentado ne
        meritas spacon en mondvasta gazeto
        <fnt>
          <bib>Monato</bib>
          <aut>Elson B. Snow</aut>
          <vrk><url ref="&Monato;/2003/008006.html">Dio, benu Usonon! (2)</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kritik.0i">kritiki</ref>,
      <ref cel="batal.vorto0i">vortobatali</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_p;&c_a;&c_d;&c_a;&c_c;&c_mol; <klr>(&c_s;&c_l;&c_o;&c_v;&c_a;&c_m;&c_ib;)</klr></trd>
  <trdgrp lng="cs">
    <trd>obo&rcaron;it se</trd>,
    <trd>osopit se</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>attaquer verbalement</trd>,
    <trd>fustiger<klr> (verbalement)</klr></trd>
  </trdgrp>
  <trd lng="id"><baz>serang</baz>menyerang scr verbal</trd>
  <trd lng="pl">atakowa&cacute; s&lstroke;ownie</trd>
  <trdgrp lng="sk">
    <trd>obori&tcaron; sa</trd>,
    <trd>osopi&tcaron; sa</trd>
  </trdgrp>
</drv>
