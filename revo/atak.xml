<drv mrk="atak.tromp0o">
  <kap>tromp<tld/>o, <var><kap>&scirc;ajn<tld/>o</kap></var></kap>
  <snc mrk="atak.tromp0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      &Scirc;ajnigata, malforta
      <ref tip="super" cel="atak.0o.batalo"><tld/>o</ref>
      por delogi malamikon,
      por provi lian rebatemon...:
      <ekz>
        post la tromp<tld/>o de kunpaktuloj, ili <klr>[&#8230;]</klr>
        komencis kontra&ubreve;<tld/>on
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#20335;&#25915; <pr>y&aacute;ngg&omacron;ng</pr></trd>
</snc>
  <trd lng="be">&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_y;&c_v;&c_a;&c_ja; <ind>&c_a;&c_t;&c_a;&c_k;&c_a;</ind></trd>
  <trd lng="de">Scheinangriff</trd>
  <trd lng="fr">fausse attaque</trd>
  <trdgrp lng="id">
    <trd>muslihat militer</trd>,
    <trd><baz>serang</baz>serangan kejutan</trd>,
    <trd><baz>serang</baz>serangan pura-pura</trd>,
