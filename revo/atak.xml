<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>atak</rad>/i
</kap>
<drv mrk="atak.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Komenci batalon kontra&ubreve; malamiko:
      <ekz>
        <tld/>i landon, popolon, urbon;
      </ekz>
      <ekz>
        la lastan el amaso <tld/>as la hundo<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;ipon rompitan &ccirc;iuj ventoj <tld/>as<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        uragano <tld/>is la tutan regionon;
      </ekz>
      <ekz>
        <tld/>o de rabistoj;
      </ekz>
      <ekz>
        <tld/>i iun per paroloj, skriboj;
      </ekz>
      <ekz>
        demando ofte <tld/>ita sed neniam solvita.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="siegx.0i">sie&gcirc;i</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Ekkapti, trafi, frapi (pri sentoj kaj malsanoj):
      <ekz>
        la febro, tremo<fnt>Z</fnt>,
        suferego<fnt>Z</fnt> lin <tld/>is;
      </ekz>
      <ekz>
        sopiro al la hejmo lin <tld/>is<fnt>Z</fnt>;
      </ekz>
      <ekz>
        teruro ilin <tld/>is
        <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Psalmaro 48, 6</lok>)</fnt>;
      </ekz>
      <ekz>
        la deziro <tld/>is min ke...;
      </ekz>
      <ekz>
        novaj, &gcirc;is nun nekonataj pensoj <tld/>is la kapon de
        la juna virino<fnt>Z</fnt>;
      </ekz>
      <ekz>
        furioza pu&scirc;o de pasio
        <tld/>is &scirc;in<fnt>Z</fnt>;
      </ekz>
      <ekz>
        krizo <tld/>is la industrion;
      </ekz>
      <ekz>
        <tld/>o de &gcirc;ojo, rido, pasio, kolero.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Ekdifekti, mordi:
      <ekz>
        rusto <tld/>as feron.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>angreifen</trd>,
    <trd>attackieren</trd>
  </trdgrp>
  <trd lng="en">attack</trd>
  <trd lng="fr">attaquer</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_t;&c_a;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_a;&c_d;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="atak.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <dif>
      Batalema, spitema:
      <ekz>
        <tld/>ema humoro;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>emo <klr>(fortego, torenteco)</klr> de la
        vento<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">angriffslustig</trd>
  <trdgrp lng="fr">
    <trd>agressif</trd>,
    <trd>querelleur</trd>
  </trdgrp>
  <trd
  lng="ru">&c_v;&c_o;&c_i;&c_n;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
