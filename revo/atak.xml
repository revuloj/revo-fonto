<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: atak.xml,v 1.31 2021/03/04 17:56:54 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>atak</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="atak.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="atak.0i.batali">
    <dif>
      Komenci batalon kontra&ubreve; malamiko:
      <ekz>
        <tld/>i landon, popolon, urbon;
      </ekz>
      <ekz>
        la lastan el amaso <tld/>as la hundo<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;ipon rompitan &ccirc;iuj ventoj <tld/>as
       <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        uragano <tld/>is la tutan regionon;
      </ekz>
      <ekz>
        <tld/>i iun per paroloj, skriboj;
      </ekz>
      <ekz>
        demando ofte <tld/>ita sed neniam solvita.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="siegx.0i">sie&gcirc;i</ref>.
    </refgrp>
    <trd lng="nl">aanvallen</trd>
    <trd lng="pt">atacar</trd>
    <trd lng="sk">za&uacute;to&ccaron;i&tcaron;</trd>
    <trd lng="tp">utala</trd>
  </snc>
  <snc mrk="atak.0i.ekkapti">
    <dif>
      Ekkapti, trafi, frapi (pri sentoj kaj malsanoj):
      <ekz>
        la febro, tremo<fnt>Z</fnt>,
        suferego<fnt>Z</fnt> lin <tld/>is;
      </ekz>
      <ekz>
        sopiro al la hejmo lin <tld/>is<fnt>Z</fnt>;
      </ekz>
      <ekz>
        teruro ilin <tld/>is
        <fnt><bib>MT</bib>, <lok>&Psa; 48:6</lok></fnt>;
      </ekz>
      <ekz>
        la deziro <tld/>is min, ke...;
      </ekz>
      <ekz>
        novaj, &gcirc;is nun nekonataj pensoj <tld/>is la kapon de
        la juna virino<fnt>Z</fnt>;
      </ekz>
      <ekz>
        furioza pu&scirc;o de pasio
        <tld/>is &scirc;in<fnt>Z</fnt>;
      </ekz>
      <ekz>
        krizo <tld/>is la industrion.
      </ekz>
    </dif>
    <trd lng="nl">overvallen</trd>
  </snc>
  <snc mrk="atak.0i.ekdifekti">
    <dif>
      Ekdifekti, mordi:
      <ekz>
        rusto <tld/>as feron.
      </ekz>
    </dif>
    <trd lng="nl">inwerken op</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_t;&c_a;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>obo&rcaron;it se (na n&ecaron;koho)</trd>,
    <trd>projevit nesouhlas</trd>,
    <trd>vystoupit <klr>(proti n&ecaron;komu)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>angreifen</trd>,
    <trd>attackieren</trd>
  </trdgrp>
  <trd lng="en">attack</trd>
  <trd lng="es">atacar</trd>
  <trd lng="fr">attaquer</trd>
  <trdgrp lng="he">
    <trd>&lamed;<ind>&tav;&qof;&vav;&fpe;</ind></trd>,
    <trd>&lamed;<ind>&he;&tav;&qof;&yod;&fpe;</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>t&aacute;mad</trd>,
    <trd>megt&aacute;mad</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>attaccare</trd>,
    <trd>assalire</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_t;&c_a;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_a;&c_d;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>napadn&uacute;&tcaron;</trd>,
    <trd>vyst&uacute;pi&tcaron; proti niekomu</trd>
  </trdgrp>
</drv>

<drv mrk="atak.0o">
  <kap><tld/>o</kap>
  <snc mrk="atak.0o.batalo">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Ago, kiu komencas batalon:
      <ekz>
        <tld/>o de rabistoj.
      </ekz>
    </dif>
  </snc>
  <snc mrk="atak.0o.ekkapto">
    <dif>
      Subita apero de emocio, afekcio, sento, malsano:
      <ekz>
        <tld/>o de &gcirc;ojo, rido, pasio, kolero.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kriz.0o">krizo</ref>.
    </refgrp>
  </snc>
  <snc mrk="atak.0o.ekdifekto">
    <dif>
      Ekdifektado de substanco.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>atak</trd>,
    <trd>ataka</trd>,
    <trd>napaden&iacute;</trd>,
    <trd>n&aacute;jezd</trd>,
    <trd>p&rcaron;epaden&iacute;</trd>,
    <trd>&uacute;tok</trd>
  </trdgrp>
  <trd lng="fr">attaque</trd>
  <trd lng="he">&#x5D4;&#x5EA;&#x5E7;&#x5E4;&#x5D4;</trd>
  <trdgrp lng="it">
    <trd>attacco</trd>,
    <trd>assaltoe</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>napadnutie</trd>,
    <trd>&uacute;tok</trd>
  </trdgrp>
</drv>

<drv mrk="atak.0anto">
  <kap><ofc>*</ofc><tld/>anto</kap>
  <snc mrk="atak.0anto.ludo">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Homo kiu atakas, dum ludo a&ubreve; sporto, kiel piedpilkado
    </dif>
  </snc>
  <snc mrk="atak.0anto.batalo">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Homo kiu <tld/>as, komencas batalon:
      <ekz>
        Kvinope ili sin &jcirc;etis sur min, sed mi venkis &ccirc;iujn kvin
        <tld/>antojn
        <fnt><bib>F</bib>, <lok>&FE; 14</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">attaquant</trd>
  <trd lng="he">&#x5EA;&#x5D5;&#x5B9;&#x5E7;&#x5B5;&#x5E3;</trd>
</drv>

<drv mrk="atak.0antino">
  <kap><tld/>antino</kap>
  <snc mrk="atak.0antino.ludo">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Virina <tld/>anto
    </dif>
  </snc>
  <snc mrk="atak.0antino.batalo">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Virino kiu <tld/>as, komencas batalon.
    </dif>
  </snc>
  <trd lng="fr">attaquante</trd>
</drv>

<drv mrk="atak.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="atak.0ema.KOMUNE">
    <dif>
      Batalema, spitema:
      <ekz>
        <tld/>ema humoro;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>emo de la ventego kaj la terura varmego
        kreskis senhalte
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 19a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_ja;&c_w;&c_n;&c_ib;&c_ch;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>agresivn&iacute;</trd>,
    <trd>&uacute;to&ccaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="de">angriffslustig</trd>
  <trdgrp lng="en">
    <trd>belligerent</trd>,
    <trd>bellicose</trd>,
    <trd>pugnacious</trd>,
    <trd>aggressive</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>agressif</trd>,
    <trd>querelleur</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&tav;&vav;&qof;&pe;&nun;&yod;</trd>,
    <trd>&tav;&vav;&qof;&fpe;</trd>,
    <trd>&lamed;&vav;&het;&mem;&nun;&yod;</trd>,
    <trd>&tav;&qof;&yod;&fpe;</trd>
  </trdgrp>
  <trd lng="hu">t&aacute;mad&oacute;kedv&udblac;</trd>
  <trdgrp lng="it">
    <trd>aggressivo</trd>,
    <trd>bellicoso</trd>
  </trdgrp>
  <trd lng="nl">aggressief</trd>
  <trd lng="ru">&c_v;&c_o;&c_i;&c_n;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>&uacute;to&ccaron;n&yacute;</trd>,
    <trd>dobyva&ccaron;n&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="atak.kontraux0o">
  <kap>kontra&ubreve;<tld/>o</kap>
  <snc mrk="atak.kontraux0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Rebata
      <ref tip="super" cel="atak.0o.batalo"><tld/>o</ref>
      kontra&ubreve; atakinta malamiko:
      <ekz>
        kvaza&ubreve; prusa stabestro, mi preparis kontra&ubreve;<tld/>on
        <fnt>
          <aut>L. Beaucaire</aut>,
          <vrk>El la vivo de bervala senta&ubreve;gulo</vrk>,
          <lok>1974</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">contre-attaque</trd>
</drv>

<drv mrk="atak.surpriz0o">
  <kap>surpriz<tld/>o</kap>
  <snc mrk="atak.surpriz0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Subita, neatendita
      <ref tip="super" cel="atak.0o.batalo"><tld/>o</ref>:
      <ekz>
        separatismaj grupoj ofte surpriz<tld/>as
        en hindian teritorion
        <fnt>
          <aut>J. Holslag</aut>,
          <vrk><url ref="http://www.vrede.be/listo-overzicht/1707-hindia-unio-terorplena-tato-n-ro-395--jan-feb-2009-">
          Hindia Unio, terorplena &scirc;tato</url></vrk>,
          <lok>Vrede, 2009-01 &gcirc;is 02 (395)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>p&rcaron;ekvapiv&yacute; &uacute;tok</trd>,
    <trd>p&rcaron;epad</trd>
  </trdgrp>
  <trd lng="fr">raid</trd>
  <trdgrp lng="it">
    <trd>blitz</trd>,
    <trd>sortita<klr> (blitz degli assediati)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>prekvapiv&yacute; &uacute;tok</trd>,
    <trd>prepad</trd>
  </trdgrp>
</drv>

<drv mrk="atak.tromp0o">
  <kap>tromp<tld/>o</kap>
  <snc mrk="atak.tromp0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      &Scirc;ajnigata, malforta
      <ref tip="super" cel="atak.0o.batalo"><tld/>o</ref>
      por delogi malamikon,
      por provi lian rebatemon...:
      <ekz>
        post la tromp<tld/>o de kunpaktuloj, ili <klr>...</klr> 
        komencis kontra&ubreve;<tld/>on
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">fausse attaque</trd>
</drv>

<drv mrk="atak.vort0i">
  <kap>vort<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="atak.vort0i.KOMUNE">
    <dif>
      <tld lit="A"/>i per malfavoraj diroj,
      tre malla&ubreve;di,
      forte <ref tip="vid" cel="kritik.0i.KOMUNE">kritiki</ref>:
      <ekz>
        &leftquot;vi estas stultulo kaj instruojn miajn vi ne 
        memoras,&rightquot; vort<tld/>is lin la maljuna sinjoro
        <fnt>
          <aut>B. N&ecaron;mcov&aacute;, trad. J. Patera</aut>,
          <vrk><url ref="http://www.literatura.bucek.name/nemcova/kaca.html">
          Diablo kaj Ka&ccirc;a</url></vrk>,
          <lok>[2009?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>obo&rcaron;it se</trd>,
    <trd>osopit se</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>attaquer verbalement</trd>,
    <trd>fustiger<klr> (verbalement)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>obori&tcaron; sa</trd>,
    <trd>osopi&tcaron; sa</trd>
  </trdgrp>
</drv>
</art>
