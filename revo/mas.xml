<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mas</rad>/o
</kap>
<drv mrk="mas.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="mas.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Baza <ref tip="super" cel="grand.fizika0o">fizika grando</ref>,
      esprimanta la kvanton da materio konsistigantan iun korpon:
      <ekz>
        la <ref cel="dimens.0o.FIZ">dimensioj</ref> de <tld/>o estas
        <frm>M<sup>0</sup>L<sup>1</sup>T<sup>0</sup></frm>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kilogr.0o">kilogramo</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Nedifinita kvanto da materio, konsiderata kiel tuta&jcirc;o, neatentante
      pri la eroj nek pri la formo:
      <ekz>
        <tld/>o de gelateno;
      </ekz>
      <ekz>
        la malluma <tld/>o de la maro
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="blok.0o">bloko</ref>,
      <ref cel="amas.0o">amaso</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">hmota</trd>
  <trd lng="de">Masse</trd>
  <trd lng="en">mass</trd>
  <trd lng="fr">masse</trd>
  <trd lng="nl">massa</trd>
  <trd lng="ru">&c_m;&c_a;&c_s;&c_s;&c_a;</trd>
</drv>

<drv mrk="mas.volumena0o">
  <kap>
    volumena <tld/>o
  </kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="grand.fizika0o">Fizika grando</ref>,
      esprimanta la
      <ref cel="rilat.0o.kvociento">rilaton</ref> de la
      <ref cel="mas.0o.FIZ">maso</ref> de certa kvanto da substanco al
      &gcirc;ia <ref cel="volume.0o">volumeno</ref>:
      <ekz>
        la volumena <tld/>o de akvo atingas maksimumon je 4&ring;C;
      </ekz>
      <ekz>
        la <ref cel="dimens.0o.FIZ">dimensioj</ref> de volumena
        <tld/>o estas <frm>ML<sup>-3</sup></frm> kaj
        la mezurunuoj estas ekz-e
        <frm>g/cm&cubic;</frm>, <frm>kg/m&cubic;</frm>, ktp.
      </ekz>
    </dif>
    <ref tip="vid" cel="dens.0eco.FIZ">denseco.</ref>
  </snc>
  <trd lng="de">Dichte</trd>
  <trd lng="en">density</trd>
  <trd lng="fr">masse volumique</trd>
  <trd lng="nl">dichtheid</trd>
  <trd lng="ru">&c_p;&c_l;&c_o;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>
</art>
