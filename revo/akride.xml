<vortaro>
<art mrk="$Id$">
<kap>
  <rad>akrided</rad>/oj
</kap>
<drv mrk="akride.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo de
      <ref tip="super" cel="ortopter.0oj">ortopteroj</ref>
      (<trd lng="la">Acrididae</trd>),
      de saltantaj insektoj, kun multegaj specioj.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_r;&c_a;&c_n;&c_ch;&c_o;&c_v;&c_y;&c_ja; &c_s;&c_a;&c_p;&c_r;&c_a;&c_w;&c_d;&c_n;&c_y;&c_ja; <klr
  tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Feldheuschrecken</trd>
  <trd lng="en">Acrididae</trd>
  <trd lng="fr">acridiens</trd>
  <trd lng="hu">s&aacute;skaf&eacute;l&eacute;k</trd>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_i;&c_je; <ind>&c_s;&c_a;&c_r;&c_a;&c_n;&c_ch;&c_o;&c_v;&c_y;&c_je;</ind> <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sv">markgr&auml;shoppor</trd>
</drv>
</art>
