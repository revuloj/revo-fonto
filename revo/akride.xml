<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: akride.xml,v 1.13 2023/10/12 16:57:43 revo Exp $">
<kap>
  <rad>akrided</rad>/oj
</kap>
<drv mrk="akride.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo de
      <ref tip="malprt" cel="ortopter.0oj">ortopteroj</ref>
      (<trd lng="la">Acrididae</trd>),
      de saltantaj insektoj, kun multegaj specioj:
      <ekz>
        Svainsona buteo <klr>[&#x2026;]</klr> ricevas anka&ubreve; la komunajn nomojn de Akrida buteo
        a&ubreve; Lokusta buteo, pro ties emo de <tld lit="A"/>oj (<ref tip="prt" cel="lokust.0o">lokustoj</ref>
        kaj <ref tip="prt" cel="akrid.0o">akridoj</ref>) kaj
        ili voras ege tiujn insektojn kiam disponeblas
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=379658">Svainsona buteo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_r;&c_a;&c_n;&c_ch;&c_o;&c_v;&c_y;&c_ja;
  &c_s;&c_a;&c_p;&c_r;&c_a;&c_w;&c_d;&c_n;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Feldheuschrecken</trd>
  <trd lng="en">Acrididae</trd>
  <trd lng="fr">acridiens</trd>
  <trd lng="he">&#x5D7;&#x5D2;&#x5D1;&#x5D9;&#x5DD;</trd>
  <trd lng="hu">s&aacute;skaf&eacute;l&eacute;k</trd>
  <trd lng="ja">&#x30D0;&#x30C3;&#x30BF;&#x79D1;  <pr>&#x3070;&#x3063;&#x305F;&#x304B;</pr></trd>
  <trd lng="pl">szara&nacute;czowate</trd>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_i;&c_je;
  <ind>&c_s;&c_a;&c_r;&c_a;&c_n;&c_ch;&c_o;&c_v;&c_y;&c_je;</ind> <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sv">markgr&auml;shoppor</trd>
</drv>
</art>
