<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>stuf</rad>/i <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="stuf.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="stuf.0i.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Longe <ref tip="super" cel="kuir.0i">kuiri</ref> en
      fermita poto, tre modere varmigante
      por konservi la tutan sukon.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_u;&c_sh;&c_y;&c_c;&c_mol;
      <klr>(&c_g;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;)</klr></trd>,
    <trd>&c_g;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol; &c_n;&c_a;
      &c_p;&c_a;&c_r;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schmoren</trd>,
    <trd>d&uuml;nsten</trd>,
    <trd>d&auml;mpfen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>stew</trd>,
    <trd>braise</trd>,
    <trd>simmer</trd>
  </trdgrp>
  <trd lng="fa">&a_A_madda;&a_r;&a_A;&a_m; &f_p;&a_kh;&a_t;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>braiser</trd>,
    <trd>cuire &agrave; l'&eacute;touff&eacute;e</trd>,
    <trd>faire mijoter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>p&aacute;rol</trd>,
    <trd>dinsztel</trd>,
    <trd>g&odblac;z&ouml;l</trd>
  </trdgrp>
  <trd lng="nl">stoven</trd>
  <trd lng="ru">&c_t;&c_u;&c_sh;&c_i;&c_t;&c_mol; <klr
tip="amb">(&c_g;&c_o;&c_t;&c_o;&c_v;&c_i;&c_t;&c_mol;)</klr></trd>
</drv>
</art>
