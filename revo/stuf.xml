<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stuf.xml,v 1.25 2025/02/21 08:31:38 revo Exp $">
<kap>
  <rad>stuf</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="stuf.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="stuf.0i.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Longe <ref tip="super" cel="kuir.0i">kuiri</ref> en
      fermita poto, tre modere varmigante
      por konservi la tutan sukon:
      <ekz>
       mi vidis denove la <tld/>itan &scirc;afa&jcirc;on kun <ref cel="ajl.0o">ajlo</ref> kaj <ref cel="kren.0o">kreno</ref>
        <fnt><aut>Henri Heine, trad. LL Zamenhof</aut>,
          <vrk>La Rabeno de Ba&hcirc;ara&hcirc;</vrk>, <lok>&ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        de kelkaj tagoj ili man&gcirc;as sen&ccirc;ese <tld/>itan grion, sen viando
        <fnt>
          <bib>LGA</bib>
          <lok>I.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>itaj frapviando-ruletoj
        <fnt>
          <bib>SrT</bib>
          <lok>Libro II.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> <tld/>is viandon super fumanta, haladza karbujeta&ccirc;o
        <fnt>
          <bib>StU</bib>
          <lok>La Nazaretanoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_u;&c_sh;&c_y;&c_c;&c_mol; <klr>(&c_g;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;)</klr></trd>,
    <trd>&c_g;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol; &c_n;&c_a; &c_p;&c_a;&c_r;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">dusit <klr>(maso)</klr></trd>
  <trdgrp lng="de">
    <trd>schmoren</trd>,
    <trd>d&uuml;nsten</trd>,
    <trd>d&auml;mpfen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>stew</trd>,
    <trd>braise</trd>,
    <trd>simmer</trd>
  </trdgrp>
  <trd lng="fa">&a_A_madda;&a_r;&a_A;&a_m; &f_p;&a_kh;&a_t;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>braiser</trd>,
    <trd>cuire &agrave; l'&eacute;touff&eacute;e</trd>,
    <trd>faire mijoter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>p&aacute;rol</trd>,
    <trd>dinsztel</trd>,
    <trd>g&odblac;z&ouml;l</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#33976;&#29038;&#12395;&#12377;&#12427;  <pr>&#12416;&#12395;&#12395;&#12377;&#12427;</pr></trd>,
    <trd>&#12392;&#12429;&#28779;&#12391;&#29038;&#12427;  <pr>&#12392;&#12429;&#12403;&#12391;&#12395;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">stoven</trd>
  <trd lng="pl">dusi&cacute;</trd>
  <trd lng="ru">&c_t;&c_u;&c_sh;&c_i;&c_t;&c_mol; <klr tip="amb">(&c_g;&c_o;&c_t;&c_o;&c_v;&c_i;&c_t;&c_mol;)</klr></trd>
  <trd lng="sk">dusi&tcaron; <klr>(m&auml;so)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_u;&c_sh;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_g;&c_o;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i; &c_n;&c_a; &c_p;&c_a;&c_r;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#29032; <pr>w&emacron;i</pr></trd>,
    <trd>&#28905; <pr>hu&igrave;</pr></trd>,
    <trd>&#29172; <pr>hu&igrave;</pr></trd>,
    <trd>&#28822; <pr>d&ugrave;n</pr></trd>,
    <trd>&#29129; <pr>d&ugrave;n</pr></trd>,
    <trd>&#33976;&#28909; <pr>zh&emacron;ngr&egrave;</pr></trd>,
    <trd>&#33976;&#29105; <pr>zh&emacron;ngr&egrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="stuf.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="stuf.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="lst" lst="voko:man&gcirc;a&jcirc;oj" cel="mangx.0ajxo.homa">Man&gcirc;a&jcirc;o</ref>
      <tld/>ita en fermita poto:
      <ekz>
       por ke la kuiristo povu fari el ili <nac>Irish Stew</nac>, irlandan
       stufa&jcirc;on...
       <fnt><bib>Mortul&Scirc;ip</bib>, <lok>&ccirc;apitro 29</lok>
       </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="pilaf.0o">pilafo</ref>,
      <ref cel="ragu.0o">raguo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_u;&c_sh;&c_a;&c_n;&c_a;&c_ja; &c_s;&c_t;&c_r;&c_a;&c_v;&c_a;</trd>,
    <trd>&c_t;&c_u;&c_sh;&c_o;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
   <trdgrp lng="de">
     <trd>Gesottenes</trd>,
     <trd>Geschmortes</trd>,
     <trd>Ged&uuml;nstetes</trd>
   </trdgrp>
   <trd lng="en">stew</trd>
   <trdgrp lng="fr">
    <trd>rago&ucirc;t</trd>,
    <trd>pot-au-feu</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#33976;&#29038;  <pr>&#12416;&#12375;&#12395;</pr></trd>,
    <trd>&#29038;&#36796;&#12415;  <pr>&#12395;&#12371;&#12415;</pr></trd>,
    <trd>&#12471;&#12481;&#12517;&#12540;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>piecze&nacute; duszona</trd>,
    <trd>sztufada</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_u;&c_sh;&c_k;&c_o;&c_v;&c_a;&c_n;&c_a; &c_s;&c_t;&c_r;&c_a;&c_v;&c_a;</trd>,
    <trd>&c_t;&c_u;&c_sh;&c_o;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="stuf.0ujo">
  <kap><tld/>ujo, <var><kap><tld/>poto</kap></var></kap>
  <snc mrk="stuf.0ujo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="pot.0o">Poto</ref>, speciale destinita por <tld/>i man&gcirc;a&jcirc;on.
      <ekz>
        li estis retirinta de la korbo sian ar&gcirc;enta&jcirc;ojn, <klr>[&#8230;]</klr> escepte de la
        <tld/>poto, kiun mi mem vartis kaj en kiu trovi&gcirc;is tri funtoj da bona viando,
        kapo de brasiko, abundaj karotoj
        <fnt>
          <aut>Erckmann-Chatrian, trad. Pieter Jan Doumen</aut>
          <vrk><url ref="http://esperanto-forum.org/doc/Romanoj/soldatodenapoleono.pdf"
              >Soldato de Napoleono</url></vrk>
          <lok>&ccirc;ap. VI, p. 87</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="marmit.0o">marmito</ref>,
      <ref cel="tajxin.0o">ta&jcirc;ino</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_o;&c_s;&c_u;&c_d; &c_d;&c_l;&c_ja; <ind>&c_t;&c_u;&c_sh;&c_e;&c_n;&c_mol;&c_n;&c_ja;</ind> <klr>(&c_s;&c_t;&c_r;&c_a;&c_v;&c_y;)</klr></trd>,
    <trd>&c_k;&c_a;&c_ch;&c_a;&c_ch;&c_n;&c_ib;&c_c;&c_a;</trd>,
    <trd>&c_g;&c_u;&c_s;&c_ja;&c_t;&c_n;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
   <trdgrp lng="de">
     <trd>Schmorpfanne</trd>,
     <trd>Schmorgef&auml;&szlig;</trd>,
     <trd>D&auml;mpfer <klr>(Topf)</klr></trd>
   </trdgrp>
   <trdgrp lng="fr">
    <trd>pot-au-feu <klr>(r&eacute;cipient)</klr></trd>,
    <trd>cocotte</trd>
  </trdgrp>
  <trd lng="pl">garnek do duszenia potraw</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_s;&c_u;&c_d;&c_i;&c_n;&c_a; &c_d;&c_l;&c_ja; &c_t;&c_u;&c_sh;&c_k;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_g;&c_u;&c_s;&c_ja;&c_t;&c_n;&c_i;&c_c;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#27801;&#37707; <pr>sh&amacron;gu&omacron;</pr></trd>,
    <trd>&#27801;&#38149; <pr>sh&amacron;gu&omacron;</pr></trd>
  </trdgrp>
</drv>
</art>
