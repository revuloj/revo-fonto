<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stuf.xml,v 1.18 2022/03/29 14:19:46 revo Exp $">
<kap>
  <rad>stuf</rad>/i <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="stuf.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="stuf.0i.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Longe <ref tip="super" cel="kuir.0i">kuiri</ref> en
      fermita poto, tre modere varmigante
      por konservi la tutan sukon:
      <ekz>
       mi vidis denove la <tld/>itan &scirc;afa&jcirc;on kun <ref cel="ajl.0o">ajlo</ref> kaj <ref cel="kren.0o">kreno</ref>
        <fnt><aut>Henri Heine, trad. LL Zamenhof</aut>,
          <vrk>La Rabeno de Ba&hcirc;ara&hcirc;</vrk>, <lok>&ccirc;apitro III</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_u;&c_sh;&c_y;&c_c;&c_mol; <klr>(&c_g;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;)</klr></trd>,
    <trd>&c_g;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol; &c_n;&c_a; &c_p;&c_a;&c_r;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">dusit <klr>(maso)</klr></trd>
  <trdgrp lng="de">
    <trd>schmoren</trd>,
    <trd>d&uuml;nsten</trd>,
    <trd>d&auml;mpfen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>stew</trd>,
    <trd>braise</trd>,
    <trd>simmer</trd>
  </trdgrp>
  <trd lng="fa">&a_A_madda;&a_r;&a_A;&a_m; &f_p;&a_kh;&a_t;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>braiser</trd>,
    <trd>cuire &agrave; l'&eacute;touff&eacute;e</trd>,
    <trd>faire mijoter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>p&aacute;rol</trd>,
    <trd>dinsztel</trd>,
    <trd>g&odblac;z&ouml;l</trd>
  </trdgrp>
  <trd lng="nl">stoven</trd>
  <trd lng="pl">dusi&cacute;</trd>
  <trd lng="ru">&c_t;&c_u;&c_sh;&c_i;&c_t;&c_mol; <klr tip="amb">(&c_g;&c_o;&c_t;&c_o;&c_v;&c_i;&c_t;&c_mol;)</klr></trd>
  <trd lng="sk">dusi&tcaron; <klr>(m&auml;so)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x442;&#x443;&#x448;&#x43A;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x433;&#x43E;&#x442;&#x443;&#x432;&#x430;&#x442;&#x438; &#x43D;&#x430; &#x43F;&#x430;&#x440;&#x456;</trd>
  </trdgrp>
</drv>

<drv mrk="stuf.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="stuf.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;a&jcirc;o <tld/>ita en fermita poto :
      <ekz>
       por ke la kuiristo povu fari el ili Irish Stew, irlandan
       stufa&jcirc;on...
       <fnt><bib>Mortul&Scirc;ip</bib>, <lok>&ccirc;apitro 29</lok>
       </fnt>  
      </ekz>
    </dif>
  </snc>
   <trd lng="en">stew</trd>
   <trdgrp lng="fr">
    <trd>rago&ucirc;t</trd>,
    <trd>pot-au-feu</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x442;&#x443;&#x448;&#x43A;&#x43E;&#x432;&#x430;&#x43D;&#x430; &#x441;&#x442;&#x440;&#x430;&#x432;&#x430;</trd>,
    <trd>&#x442;&#x443;&#x448;&#x43E;&#x43D;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>

<drv mrk="stuf.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="stuf.0ujo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Poto, speciale destinita por <tld/>i man&gcirc;a&jcirc;on
    </dif>
  </snc>
   <trdgrp lng="fr">
    <trd>pot-au-feu <klr>(r&eacute;cipient)</klr></trd>,
    <trd>cocotte</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x441;&#x443;&#x434;&#x438;&#x43D;&#x430; &#x434;&#x43B;&#x44F; &#x442;&#x443;&#x448;&#x43A;&#x443;&#x432;&#x430;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x433;&#x443;&#x441;&#x44F;&#x442;&#x43D;&#x438;&#x446;&#x44F;</trd>
  </trdgrp>
</drv>

</art>
