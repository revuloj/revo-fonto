<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kalikl</rad>/o
</kap>
<drv mrk="kalikl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Verticelo da folioloj &ccirc;irka&ubreve;anta la kalikon &ccirc;e multaj floroj kaj
      formanta kvaza&ubreve; duan kalikon.
    </dif>
  </snc>
<trd lng="nl">bijkelk</trd>
<trd lng="pl">okwiat</trd>
</drv>
<drv mrk="kalikl.0a">
  <kap><tld/>(ohav)a</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Havanta <tld/>on.  
    </dif>
  </snc>
</drv>
</art>

