<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kalikl.xml,v 1.17 2023/10/18 14:48:09 revo Exp $">
<kap>
  <rad>kalikl</rad>/o
</kap>
<drv mrk="kalikl.0o">
  <kap><tld/>o</kap>
  <fnt><bib>PV</bib></fnt>
  <snc mrk="kalikl.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="dif" cel="kalik.krom0o">Kromkaliko</ref>:
      <ekz>
        &ccirc;e la malvoj kaj skabiozoj la kalikhavajn florojn aldone &ccirc;irka&ubreve;as <tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>H&uuml;llkelch </trd>,
      <trd>Au&szlig;enkelch</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_ch;&c_a;&c_sh;&c_sh;&c_a;</trd>
  <trd lng="ca">calicle</trd>
  <trdgrp lng="en"><trd>epicalyx</trd>, <trd>calycle</trd></trdgrp>
  <trd lng="fr">calicule</trd>
  <trdgrp lng="hu">
    <trd>k&uuml;ls&odblac; cs&eacute;sze</trd>,
    <trd>&aacute;lcs&eacute;sze</trd>
  </trdgrp>
  <trd lng="ja">&#x526F;&#x843C;  <pr>&#12405;&#x304F;&#x304C;&#x304F;</pr></trd>
  <trd lng="nl">bijkelk</trd>
  <trd lng="pl">okwiat</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_ch;&c_a;&c_sh;&c_i;&c_je;</trd>,
    <trd>&c_e;&c_p;&c_i;&c_k;&c_a;&c_l;&c_i;&c_k;&c_s;</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&c_ib;&c_d;&c_ch;&c_a;&c_sh;&c_sh;&#x44F;</trd>
</drv>

<drv mrk="kalikl.0a">
  <kap><tld/>a, <var><kap><tld/>ohava</kap></var></kap>
  <fnt><bib>PV</bib></fnt>
  <snc mrk="kalikl.0a.BOT">
     <uzo tip="fak">BOT</uzo>
    <dif>
      Havanta <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_z; <ind>&c_p;&c_a;&c_d;&c_ch;&c_a;&c_sh;&c_sh;&c_a;&c_m;</ind></trd>
  <trd lng="ca">caliculat</trd>
  <trd lng="en">calycular</trd>
  <trd lng="fr">calicul&eacute;</trd>
  <trd lng="ja">&#x526F;&#x843C;&#12434;&#25345;&#12387;&#x305F;  <pr>&#12405;&#x304F;&#x304C;&#x304F;&#12434;&#12418;&#12387;&#x305F;</pr></trd>
  <trd lng="nl">een <ind>bijkelk</ind> hebbend</trd>
  <trd lng="uk">&c_z; &#x43F;&c_ib;&c_d;&c_ch;&c_a;&c_sh;&c_sh;&#x44F;&#x43C;</trd>
</drv>
</art>
