<vortaro>
<art mrk="$Id$">
<kap>
<rad>begoni</rad>/o
</kap>
<drv mrk="begoni.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super"
      cel="begoni1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Begonia</trd>), kun nesimetriaj folioj.
      Multaj plej ofte tropikaj specioj, el kiuj pluraj estas
      &gcirc;ardene
      kaj &ccirc;ambre kulturataj.
    </dif>
  </snc>
<bld lok="../bld/begoni.jpg">&ccirc;ambra <tld/>o</bld>
  <trd lng="be">&c_b;&c_e;&c_g;&c_o;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="en">begonia</trd>
  <trdgrp lng="de">
    <trd>Begonie</trd>,
    <trd>Schiefblatt</trd>
  </trdgrp>
  <trd lng="es">begonia</trd>
  <trd lng="fr">b&eacute;gonia</trd>
  <trd lng="hu">beg&oacute;nia</trd>
  <trd lng="nl">begonia</trd>
  <trd lng="pl">begonia</trd>
  <trd lng="ru">&c_b;&c_je;&c_g;&c_o;&c_n;&c_i;&c_ja;</trd>
  <trd lng="sv">begonia</trd>
</drv>

</art>
