<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>begoni</rad>/o
</kap>
<drv mrk="begoni.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="begoni.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Begonia</trd>), kun nesimetriaj folioj. 
      Multaj plej ofte tropikaj specioj, el kiuj pluraj estas &gcirc;ardene
      kaj &ccirc;ambre kulturataj.
    </dif>
  </snc>
  <bld lok="../bld/begoni.jpg">&ccirc;ambra <tld/>o</bld>
  <trdgrp lng="de">
    <trd>Begonie</trd>,
    <trd>Schiefblatt</trd>
  </trdgrp>
  <trd lng="nl">begonia</trd>
</drv>
<drv mrk="begoni.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de vegeta&jcirc;oj (<trd lng="la">Begoniaceae</trd>),
      kun nesimetriaj folioj.
    </dif>
  </snc>
  <trd lng="de">Schiefblattgew&auml;chse</trd>
</drv>
</art>
