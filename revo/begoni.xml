<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: begoni.xml,v 1.24 2023/08/31 08:48:50 revo Exp $">
<kap>
<rad>begoni</rad>/o
</kap>
<drv mrk="begoni.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="begoni1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Begonia</trd>), kun nesimetriaj folioj;
      multaj plej ofte tropikaj specioj, el kiuj pluraj estas
      &gcirc;ardene kaj &ccirc;ambre kulturataj:
      <ekz>
        la <nom>Kimjongilia</nom> estas <klr>[&#x2026;]</klr> hibrido de <tld/>o tuberosa, <klr>[&#x2026;]</klr>
        kiam <nom>Kim Jong-il</nom> mortis en decembro de la 2011 la floro estis uzita por garni
        <klr>[l]</klr>ian kadavron dum la publika elmontrado
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=554589">Kimjongilia</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/begoni.jpg">&ccirc;ambra <tld/>o</bld>
  <trd lng="be">&c_b;&c_e;&c_g;&c_o;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_b;&c_je;&c_g;&c_o;&c_n;&c_i;&c_ja;</trd>
  <trd lng="ca">beg&ograve;nia</trd>
  <trdgrp lng="de">
    <trd>Begonie</trd>,
    <trd>Schiefblatt</trd>
  </trdgrp>
  <trd lng="en">begonia</trd>
  <trd lng="es">begonia</trd>
  <trd lng="fr">b&eacute;gonia</trd>
  <trd lng="hu">beg&oacute;nia</trd>
  <trdgrp lng="ja">
    <trd>&#x30D9;&#x30B4;&#x30CB;&#x30A2;</trd>,
    <trd>&#x30B7;&#x30E5;&#x30A6;&#x30AB;&#x30A4;&#x30C9;&#x30A6;</trd>
  </trdgrp>
  <trd lng="nl">begonia</trd>
  <trd lng="pl">begonia</trd>
  <trd lng="ru">&c_b;&c_je;&c_g;&c_o;&c_n;&c_i;&c_ja;</trd>
  <trd lng="sv">begonia</trd>
  <trd lng="sk">beg&oacute;nia</trd>
  <trd lng="cs">beg&oacute;nie</trd>
  <trd lng="uk">&#x431;&#x435;&#x433;&#x43E;&#x43D;&#x456;&#x44F;</trd>
</drv>

</art>
