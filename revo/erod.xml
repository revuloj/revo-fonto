<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>erod</rad>/i
  <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="erod.0i">
  <kap><tld/>i</kap>
  <snc mrk="erod.0i.KOMUNE">
    <dif>
      <ref tip="dif" cel="erozi.0i">Erozii</ref>.
      <ekz>
        la plej bonaj el <klr>[la domoj]</klr> aspektis malnovaj kaj iomete
        <tld/>itaj de la vetero
        <fnt><bib>DOz</bib> <lok>Mirinda eskapo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_r;&c_a;&c_z;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_m;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_je;&c_r;&c_o;&c_d;&c_i;&c_r;&c_a;&c_m;</trd>
  <trd lng="ca">erosionar</trd>
  <trdgrp lng="de">
    <trd>erodieren</trd>,
    <trd>abtragen</trd>,
    <trd>auswaschen</trd>,
    <trd>abschwemmen</trd>
  </trdgrp>
  <trd lng="en">erode</trd>
  <trd lng="es">erosionar</trd>
  <trd lng="fa">&a_f;&a_r;&a_s;&a_w;&a_d;&a_n;</trd>
  <trd lng="fr">&eacute;roder</trd>
  <trdgrp lng="hu">
    <trd>erod&aacute;l</trd>,
    <trd>lehord</trd>,
    <trd>lepuszt&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">eroderen</trd>
  <trd lng="pl">erodowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>desgastar</trd>,
    <trd>esmerilar</trd>,
    <trd>erodir</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_m;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">erodera<klr> (n&aring;got)</klr></trd>
  <trdgrp lng="sk">
    <trd>erodova&tcaron;</trd>,
    <trd>rozr&yacute;va&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>rozm&ecaron;l&ncaron;ovat</trd>,
    <trd>rozt&iacute;rat</trd>
  </trdgrp>
</drv>

<drv mrk="erod.0o">
  <kap><tld/>o</kap>
  <snc mrk="erod.0o.KOMUNE">
    <dif>
      <ref tip="dif" cel="erozi.0o">Erozio</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_r;&c_o;&c_z;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_je;&c_r;&c_o;&c_z;&c_i;&c_ja;</trd>
  <trd lng="ca">erosi&oacute;</trd>
  <trdgrp lng="de">
    <trd>Erosion</trd>,
    <trd>Abtragung</trd>,
    <trd>Abschwemmung</trd>
  </trdgrp>
  <trd lng="en">erosion</trd>
  <trd lng="es">eroci&oacute;n</trd>
  <trd lng="fa">&a_f;&a_r;&a_s;&a_A;&f_y;&a_sh;</trd>
  <trd lng="fr">&eacute;rosion</trd>
  <trdgrp lng="hu">
    <trd>er&oacute;zi&oacute;</trd>,
    <trd>lehord&aacute;s</trd>,
    <trd>lepusztul&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">erosie</trd>
  <trd lng="pl">erozja</trd>
  <trd lng="ru">&c_e;&c_r;&c_o;&c_z;&c_i;&c_ja;</trd>
  <trd lng="sv">erosion</trd>
  <trdgrp lng="sk">
    <trd>er&oacute;zia</trd>,
    <trd>vymie&lcaron;anie</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>eroze</trd>,
    <trd>vym&iacute;l&aacute;n&iacute;</trd>
  </trdgrp>
</drv>

<drv mrk="erod.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="erod.0igxi.KOMUNE">
    <dif>
      Foruzi&gcirc;i, trivi&gcirc;i:
      <ekz>
        la milit&scirc;ipoj da&ubreve;re ekzercis sin per pafado
        al imagataj malamikoj, kaj &ccirc;iufoje la paftuboj
        de la kanonoj iom <tld/>i&gcirc;is
        <fnt>
          <aut>T. Steele</aut>
          <vrk>Konvinka Kamufla&jcirc;o</vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">s'<ind>&eacute;roder</ind></trd>
</drv>
</art>

