<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>erod</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="erod.0i">
  <kap><tld/>i</kap>
  <ref tip="dif" cel="erozi.0i">erozii</ref>
  <ekz>
    la plej bonaj el <klr>[la domoj]</klr> aspektis malnovaj kaj iomete
    <tld/>itaj de la vetero
    <fnt><bib>DOz</bib>, <lok>&leftquot;mirinda
eskapo&rightquot;</lok></fnt>.
  </ekz>
  <trdgrp lng="be">
    <trd>&c_e;&c_r;&c_a;&c_z;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_m;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>erodieren</trd>,
    <trd>abtragen</trd>,
    <trd>auswaschen</trd>,
    <trd>abschwemmen</trd>
  </trdgrp>
  <trd lng="es">erosionar</trd>
  <trd lng="fr">&eacute;roder</trd>
  <trdgrp lng="hu">
    <trd>erod&aacute;l</trd>,
    <trd>lehord</trd>,
    <trd>lepuszt&iacute;t</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_m;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">erodera<klr> (n&aring;got)</klr></trd>
</drv>

<drv mrk="erod.0o">
  <kap><tld/>o</kap>
  <ref tip="dif" cel="erozi.0o">erozio</ref>
  <trd lng="be">&c_e;&c_r;&c_o;&c_z;&c_ib;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Erosion</trd>,
    <trd>Abtragung</trd>,
    <trd>Abschwemmung</trd>
  </trdgrp>
  <trd lng="es">eroci&oacute;n</trd>
  <trd lng="fr">&eacute;rosion</trd>
  <trdgrp lng="hu">
    <trd>er&oacute;zi&oacute;</trd>,
    <trd>lehord&aacute;s</trd>,
    <trd>lepusztul&aacute;s</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_r;&c_o;&c_z;&c_i;&c_ja;</trd>
  <trd lng="sv">erosion</trd>
</drv>
</art>

