<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Gustav</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="gustav.0o">
  <kap><tld/>o</kap>
  <snc mrk="gustav.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira anta&ubreve;nomo latindevena:
      <ekz>
        iun vesperon li vizitis <tld/>on kaj plikonati&gcirc;is kun liaj 
        interesoj kaj okupoj
        <fnt>
          <aut>Stellan Engholm</aut>,
          <vrk>Vivo vokas</vrk>,
          <lok>1946</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Gustav</trd>,
    <trd>Gustaf</trd>
  </trdgrp>
  <trd lng="fa">&f_g;&a_w;&a_s;&a_t;&a_A;&a_w;</trd>
  <trd lng="fr">Gustave</trd>
  <trd lng="pl">Gustaw</trd>
  <trd lng="pt">Gustavo</trd>
  <trd lng="ru">&c_G;&c_u;&c_s;&c_t;&c_a;&c_v;</trd>
</drv>
</art>

