<drv mrk="fajr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fajr.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Fenomeno konsistanta en brulkonsumado de diversaj
      substancoj, kun produktado de lumo kaj varmo:
      <ekz>
        fari <klr>(ekbruligi)</klr> <tld/>on
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz><ekz>
        bloveksciti la <tld/>on
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;irka&ubreve;a&jcirc;o <klr>[&#x2026;]</klr> dezertigita per <tld/>o kaj glavo
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz><ekz>
        tro akra <tld/>o estas sen da&ubreve;ro
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flam.0o">flamo</ref>,
      <ref cel="ard">ardo</ref>
    </refgrp>
    <trd lng="bo">&t_ma;&t_drengbu;&t_tsheg;</trd>
    <trd lng="br">tan</trd>
    <trd lng="ca">foc</trd>
    <trd lng="en">fire</trd>
    <trd lng="ja">&#x706B; [<ind>&#x3072;</ind>]</trd>
    <trd lng="pl">ogie&nacute;</trd>
    <trdgrp lng="pt">
      <trd>fogo</trd>,
      <trd>lume</trd>
    </trdgrp>
    <trd lng="tok">seli</trd>
    <trd lng="vo">fil</trd>
  </snc>
  <snc mrk="fajr.0o.fervoro">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Entuziasmo, granda fervoro, ardeco:
      <ekz>
        en la <tld/>o de mia kolero
        <fnt><bib>MT</bib>, <lok>&Jeh; 38:19</lok></fnt>;
      </ekz><ekz>
        diskuti kun <tld/>o;
      </ekz>
      <ekz>
        nun, &#x2010; ekkriis li kun <tld/>o, &#x2010; nun mi havas celon en la vivo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i reblovas en li la <tld/>on de l&#x2019; ven&gcirc;emo
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ard.0o">ardo</ref>,
      <ref cel="ekzalt">ekzalteco</ref>,
      <ref cel="entuzi.0o">entuziasmo</ref>,
      <ref cel="febr.0o.fervoro">febro<sncref/></ref>,
      <ref cel="fervor.0o">fervoro</ref>,
      <ref cel="flam.0o.fervoro">flamo<sncref/></ref>,
      <ref cel="pasi.0o">pasio</ref>,
      <ref cel="varm.0o">varmo</ref>,
      <ref cel="verv.0o">vervo</ref>,
      <ref cel="vigl">vigleco</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="incit.0i">inciti</ref>.
    </refgrp>
    <trdgrp lng="br">
      <trd>entan</trd>,
      <trd>birvidigezh</trd>,
      <trd>gred</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>entusiasme</trd>,
      <trd>passi&oacute;</trd>,
      <trd>enardiment</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>ardor</trd>,
      <trd>enthusiasm</trd>,
      <trd>fervor</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>ogie&nacute;</trd>,
      <trd>zapa&lstroke;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>fogo</trd>,
      <trd>entusiasmo</trd>
    </trdgrp>
  </snc>
