  </snc>
  <trdgrp lng="ca">
      <trd>molls</trd>,
      <trd>tenalles</trd>
    </trdgrp>
  <trd lng="cs">kle&scaron;t&ecaron; na &zcaron;hav&eacute; d&rcaron;evo v krbu</trd>
  <trd lng="de">Feuerzange</trd>
  <trd lng="en">fire <ind>tongs</ind></trd>
  <trd lng="fr">pince &agrave; feu</trd>
  <trd lng="hu">t&udblac;zfog&oacute;</trd>
  <trd lng="nl">vuurtang</trd>
  <trd lng="pl">szczypce do w&eogonek;gli</trd>
  <trd lng="ru">&c_shch;&c_i;&c_p;&c_c;&c_y; &c_d;&c_l;&c_ja;
