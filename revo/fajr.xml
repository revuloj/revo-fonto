<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fajr.xml,v 1.42 2020/01/09 19:10:19 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fajr</rad>/o
</kap>

<drv mrk="fajr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fajr.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Fenomeno konsistanta en brulkonsumado de diversaj
      substancoj, kun produktado de lumo kaj varmo:
      <ekz>
        fari <klr>(ekbruligi)</klr> <tld/>on
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz><ekz>
        bloveksciti la <tld/>on
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;irka&ubreve;a&jcirc;o <klr>[&#8230;]</klr> dezertigita per <tld/>o kaj glavo
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz><ekz>
        tro akra <tld/>o estas sen da&ubreve;ro
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flam.0o">flamo</ref>,
      <ref cel="ard">ardo</ref>
    </refgrp>
    <trd lng="br">tan</trd>
    <trd lng="ca">foc</trd>
    <trd lng="en">fire</trd>
    <trd lng="ja">&#28779; [<ind>&#12402;</ind>]</trd>
    <trd lng="pl">ogie&nacute;</trd>
    <trdgrp lng="pt">
      <trd>fogo</trd>,
      <trd>lume</trd>
    </trdgrp>
    <trd lng="tp">seli</trd>
    <trd lng="vo">fil</trd>
  </snc>
  <snc mrk="fajr.0o.fervoro">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Entuziasmo, granda fervoro, ardeco:
      <ekz>
        en la <tld/>o de mia kolero
        <fnt><bib>MT</bib>, <lok>&Jeh; 38:19</lok></fnt>;
      </ekz><ekz>
        diskuti kun <tld/>o;
      </ekz>
      <ekz>
        nun, &dash; ekkriis li kun <tld/>o, &dash; nun mi havas celon en la vivo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i reblovas en li la <tld/>on de l&#8217; ven&gcirc;emo
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ard.0o">ardo</ref>,
      <ref cel="ekzalt">ekzalteco</ref>,
      <ref cel="entuzi.0o">entuziasmo</ref>,
      <ref cel="febr.0o.fervoro">febro<sncref/></ref>,
      <ref cel="fervor.0o">fervoro</ref>,
      <ref cel="flam.0o.fervoro">flamo<sncref/></ref>,
      <ref cel="pasi.0o">pasio</ref>,
      <ref cel="varm.0o">varmo</ref>,
      <ref cel="verv.0o">vervo</ref>,
      <ref cel="vigl">vigleco</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="incit.0i">inciti</ref>.
    </refgrp>
    <trdgrp lng="br">
      <trd>entan</trd>,
      <trd>birvidigezh</trd>,
      <trd>gred</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>entusiasme</trd>,
      <trd>passi&oacute;</trd>,
      <trd>enardiment</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>ardor</trd>,
      <trd>enthusiasm</trd>,
      <trd>fervor</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>ogie&nacute;</trd>,
      <trd>zapa&lstroke;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>fogo</trd>,
      <trd>entusiasmo</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_a;&c_g;&c_o;&c_n;&c_mol;</trd>
  <trd lng="bg">&c_o;&c_g;&c_malmol;&c_n;</trd>
  <trd lng="ca">foc</trd>
  <trd lng="cs">ohe&ncaron;</trd>
  <trd lng="de">Feuer</trd>
  <trd lng="es">fuego</trd>
  <trd lng="fr">feu</trd>
  <trd lng="hu">t&udblac;z</trd>
  <trd lng="id">api</trd>
  <trd lng="nl">vuur</trd>
  <trd lng="ru">&c_o;&c_g;&c_o;&c_n;&c_mol;</trd>
  <trd lng="sk">ohe&ncaron;</trd>
  <trd lng="sv">eld</trd>
  <trd lng="tr">ate&scedil;</trd>
  <trd lng="zh">&#28779;<klr> [hu&#466;]</klr></trd>
</drv>

<drv mrk="fajr.0a">
  <kap><tld/>a</kap>
  <snc mrk="fajr.0a.rilata">
    <dif>
      De <tld/>o, simila al <tld/>o, havanta econ de <tld/>o:
      <ekz>
        <tld/>a flamo
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 15:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; la trono brulis sep <tld/>aj tor&ccirc;oj
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 4:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a kolero
        <fnt>
          <bib>NT</bib>
          <lok>Hebreoj 10:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>aj kisoj
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;i ne rimarkis liajn <tld/>ajn mallumajn okulojn
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        nigraj <tld/>aj &ccirc;evaloj blekis en la stalo
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_o;&c_g;&c_n;&c_je;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_g;&c_n;&c_ib;&c_s;&c_t;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_y;&c_m;&c_ja;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>entanet</trd>,
  </trdgrp>
  <trdgrp lng="ca">
    <trd>igni</trd>,
    <trd>de foc</trd>,
    <trd>ardent</trd>,
    <trd>incandescent</trd>
  </trdgrp>
  <trd lng="cs">ohniv&yacute;</trd>
  <trdgrp lng="en">
    <trd>burning</trd>,
    <trd>fiery</trd>
  </trdgrp>
  <trd lng="es">fogoso</trd>
  <trdgrp lng="fr">
    <trd>de <ind>feu</ind></trd>,
    <trd>fougueux</trd>,
    <trd>ign&eacute;</trd>,
    <trd>enflamm&eacute;<klr tip="ind">
    (passionn&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="hu">t&uuml;zes</trd>
  <trd lng="id">[<ind>api</ind>] berapi</trd>
  <trd lng="nl">vurig</trd>
  <trd lng="pl">ognisty</trd>
  <trd lng="ru">&c_o;&c_g;&c_n;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">oh&ncaron;ov&yacute;</trd>
  <trd lng="tr">ate&scedil;li</trd>
</drv>

<drv mrk="fajr.0e">
  <kap><tld/>e</kap>
  <snc mrk="fajr.0e.maniero">
    <dif>
      En <tld/>a maniero:
      <ekz>
        <tld/>e ru&gcirc;aj floroj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>e ami, paroli;
      </ekz>
      <ekz>
        <tld/>e brulis liaj &ccirc;irka&ubreve;prenoj / forte batis koro &ccirc;e la koro
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_y;&c_m;&c_ja;&c_n;&c_a;</trd>
  <trdgrp lng="br">
      <trd>gant <ind>entan</ind></trd>,
      <trd>gant <ind>birvidigezh</ind></trd>,
      <trd>gant <ind>gred</ind></trd>
    </trdgrp>
  <trdgrp lng="ca">
      <trd>apassionadament</trd>,
      <trd>ardentment</trd>
    </trdgrp>
  <trd lng="cs">ohniv&ecaron;</trd>
  <trd lng="de">feurig</trd>
  <trdgrp lng="fr">
      <trd>avec <ind>feu</ind></trd>,
      <trd>avec <ind>fougue</ind></trd>,
      <trd>fougueusement</trd>,
      <trd>passionn&eacute;ment</trd>
    </trdgrp>
  <trd lng="hu">t&uuml;zesen</trd>
  <trd lng="nl">vurig</trd>
  <trd lng="pl">ogni&sacute;cie</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_m;&c_je;&c_n;&c_n;&c_o;</trd>
  <trd lng="sk">ohnivo</trd>
  <trdgrp lng="tr">
      <trd>ate&scedil;le</trd>,
      <trd>tutkuyla</trd>
    </trdgrp>
</drv>

<drv mrk="fajr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="fajr.0i.KOMUNE">
    <dif>
      Bruli kun a&ubreve; kiel <tld/>o:
      <ekz>
        vespere, la 1-an de julio 1915, la &ccirc;ielo ru&gcirc;e <tld/>is &ccirc;irka&ubreve; Varsovio
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kolero <tld/>as en okuloj <klr>[&#8230;]</klr>
        <fnt>
          <bib>VivZam</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_e;&c_c;&c_mol;</trd>
  <trdgrp lng="br">
      <trd>flamma&ntilde;</trd>,
      <trd>flammenni&ntilde;</trd>,
      <trd>flammina&ntilde;</trd>
    </trdgrp>
  <trd lng="ca">cremar</trd>
  <trdgrp lng="cs">
  <trd>fajrovat</trd>,
  <trd>ho&rcaron;et</trd>,
  <trd>pl&aacute;t</trd>
  </trdgrp>
  <trdgrp lng="en">
      <trd>burn</trd>,
      <trd>flame</trd>
    </trdgrp>
  <trd lng="es">arder</trd>
  <trd lng="fr">flamber</trd>
  <trdgrp lng="hu">
      <trd>&eacute;g</trd>,
      <trd>t&uuml;zel<klr tip="ind"> (&eacute;g)</klr></trd>
    </trdgrp>
  <trdgrp lng="nl">
      <trd>aansteken</trd>,
      <trd>flamberen</trd>
    </trdgrp>
  <trdgrp lng="pl">
      <trd>p&lstroke;on&aogonek;&cacute;</trd>,
      <trd>gorze&cacute;<klr> (przest.)</klr></trd>,
      <trd>pali&cacute; si&eogonek;</trd>
    </trdgrp>
  <trdgrp lng="ru">
      <trd>&c_g;&c_o;&c_r;&c_je;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_l;&c_a;&c_m;&c_je;&c_n;&c_je;&c_t;&c_mol;</trd>
    </trdgrp>
  <trd lng="sk">horie&tcaron;</trd>
  <trd lng="tr">alev</trd>
</drv>

<drv mrk="fajr.0ero">
  <kap><ofc>*</ofc><tld/>ero</kap>
  <snc mrk="fajr.0ero.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Arda parteto el&jcirc;etita de brulanta korpo, produktata de
      du kunfrotaj malmolaj korpoj, a&ubreve; eligata de <ref cel="elektr.0umi">elektrumita</ref> korpo:
      <ekz>
        unu <tld/>ero estas sufi&ccirc;a, por eksplodigi pulvon
        <fnt><bib>F</bib>, <lok>&FE; 41</lok></fnt>.
      </ekz>
      <ekz>
        apena&ubreve; li ekbatis <klr>[&#8230;]</klr> kaj la <tld/>eroj ekflugis el la <tld/>ilo
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;talo ekfrapis la <tld/>o&scirc;tonon tiel, ke <tld/>eroj ek&scirc;prucis
        <fnt>
          <bib>Fab1</bib>
          <lok>Fluganta kofro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lumo de la suno estingi&gcirc;is kiel lasta <tld/>ero de brulanta papero
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        brilanta, <tld/>eranta stelo
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>erantaj okuloj
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sur la fundo de lia timo veki&gcirc;is <tld/>ero de naiva espero
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="spark.0o">sparko</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="elektr.0afajrero">elektra <tld/>ero</ref>
    </refgrp>
    <adm>
      Vidu administran noton sub &leftquot;elektr&rightquot; pri &leftquot;elektrumi&rightquot;. Cetere
      &leftquot;elektra fajrero&rightquot; pli bone ku&scirc;us &ccirc;i tie ol sub &leftquot;elektr&rightquot;.
      <aut>MB</aut>
    </adm>
  </snc>
  <trd lng="be">&c_ib;&c_s;&c_k;&c_r;&c_a;</trd>
  <trdgrp lng="br">
    <trd>elf</trd>,
    <trd>fulenn<klr tip="ind"> (tan)</klr></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>espurna</trd>,
    <trd>guspira</trd>
  </trdgrp>
  <trd lng="de">Funke</trd>
  <trd lng="en">spark</trd>
  <trd lng="es">chispa</trd>
  <trd lng="fr">&eacute;tincelle</trd>
  <trd lng="hu">szikra</trd>
  <trd lng="ja">&#28779;&#33457;
  [<ind>&#12402;&#12400;&#12394;</ind>]</trd>
  <trd lng="nl">vonk</trd>
  <trdgrp lng="pl">
    <trd>iskra</trd>,
    <trd>skra</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_s;&c_k;&c_r;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#28779;&#26143;<klr> [hu&#466;x&#299;ng]</klr></trd>,
    <trd>&#28779;&#26143;&#20799;</trd>,
    <trd><klr>tr. </klr>&#28779;&#26143;&#20818;<klr>
        [hu&#466;x&#299;ngr]</klr></trd>,
    <trd>&#26143;&#28779;<klr> [x&#299;nghu&#466;]</klr></trd>,
    <trd>&#28779;&#33457;<klr> [hu&#466;hu&#257;]</klr></trd>
  </trdgrp>
  <trd lng="tr">k&inodot;v&inodot;lc&inodot;m</trd>
  <trd lng="sk">iskra</trd>
<trd lng="cs">jiskra z ohn&ecaron;</trd>
</drv>

<drv mrk="fajr.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fajr.0igi.KOMUNE">
    <dif>
      <ref tip="dif" cel="brul.ek0igi">Ekbruligi</ref>:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        koncepto de unuetna &scirc;tato <klr>[&#8230;]</klr> <tld/>igis Balkanion en la lastaj 200
        jaroj
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="https://www.monato.be/2007/009139.php">Serioze revizii la historion</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        certe &ccirc;io, kion oni rakontis estis nur fabelo, kiu
        <tld/>igis la imagojn
        <fnt>
          <aut>J. Modest</aut>,
          <vrk>La ikono</vrk>,
          <bib>Monato</bib>,
          <lok>2001:12, p. 23a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_p;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="br">
      <trd>tana&ntilde;</trd>,
      <trd>entana&ntilde;</trd>,
      <trd>c'hwezha&ntilde;<klr> (an tan)</klr></trd>
    </trdgrp>
  <trdgrp lng="ca">
      <trd>encendre</trd>,
      <trd>inflamar</trd>,
      <trd>calar foc</trd>,
      <trd>incendiar</trd>
    </trdgrp>
  <trd lng="cs">zap&aacute;lit</trd>
  <trdgrp lng="de">
      <trd>anz&uuml;nden</trd>,
      <trd>entz&uuml;nden</trd>,
      <trd>entflammen</trd>,
      <trd>entfachen</trd>
    </trdgrp>
  <trd lng="es">darle fuego a</trd>
  <trdgrp lng="fr">
      <trd>allumer <klr tip="ind">(mettre le feu
      &agrave;)</klr></trd>,
      <trd>mettre le <ind>feu</ind> (&agrave;)</trd>
    </trdgrp>
  <trdgrp lng="hu">
      <trd>l&aacute;ngra lobbant</trd>,
      <trd>meggy&uacute;jt</trd>
    </trdgrp>
  <trd lng="nl">aansteken</trd>
  <trdgrp lng="pl">
      <trd>rozpala&cacute;</trd>,
      <trd>zapala&cacute;</trd>
    </trdgrp>
  <trdgrp lng="pt">
      <trd>atear <klr>(fogo)</klr></trd>,
      <trd>acender <klr>(o fogo)</klr></trd>,
      <trd>tacar <klr>(fogo)</klr></trd>
    </trdgrp>
  <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_zh;&c_i;&c_g;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_zh;&c_je;&c_ch;&c_mol;</trd>
    </trdgrp>
  <trd lng="sk">zap&aacute;li&tcaron;</trd>
  <trd lng="tr">ate&scedil; yakmak</trd>
</drv>

<drv mrk="fajr.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="fajr.0ilo.KOMUNE">
    <dif>
      Malgranda ilo por produkti <tld/>erojn per frotado de
      silik&scirc;tono kaj &scirc;talpeco, a&ubreve; per alia procedo:
      <ekz>
        trovi&gcirc;as ankora&ubreve; peceto da me&ccirc;o en la <tld/>ilo
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        el sia po&scirc;o prenis <tld/>ilon 23-jara&gcirc;a studento kaj ekbruligis la dokumenton
        <fnt>
          <bib>Monato</bib>
          <aut>last/pg</aut>
          <vrk><url ref="https://www.monato.be/2006/009030.php">Fajra afero</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        li prenas la <tld/>ilon kaj bruligas la cigaredon
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="https://www.monato.be/2013/011110.php">Splito</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_p;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="br">direnn</trd>
  <trdgrp lng="ca">
      <trd>encenedor</trd>,
      <trd>mistera</trd>
    </trdgrp>
  <trdgrp lng="cs">
  <trd>k&rcaron;esadlo</trd>,
  <trd>zapalova&ccaron;</trd>
  </trdgrp>
  <trd lng="de">Feuerzeug</trd>
  <trd lng="en">lighter</trd>
  <trd lng="es">encendedor</trd>
  <trd lng="fr">briquet</trd>
  <trdgrp lng="hu">
      <trd>t&udblac;zszersz&aacute;m</trd>,
      <trd>&ouml;ngy&uacute;jt&oacute;</trd>
    </trdgrp>
  <trd lng="id"><ind>korek</ind> api</trd>
  <trd lng="nl">aansteker</trd>
  <trd lng="pl">krzesiwo</trd>
  <trd lng="pt">isqueiro</trd>
  <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_je;&c_s;&c_a;&c_l;&c_o;</trd>,
      <trd>&c_z;&c_a;&c_zh;&c_i;&c_g;&c_a;&c_l;&c_k;&c_a;</trd>
    </trdgrp>
  <trd lng="sk">zapa&lcaron;ova&ccaron;</trd>
</drv>

<drv mrk="fajr.0obirdo">
  <kap><tld/>obirdo</kap>
  <snc mrk="fajr.0obirdo.rusa">
   <uzo tip="fak">MIT</uzo>
    <dif>
      Folklora birdo de fora lando, simila al pavo kun <tld/>aj
      plumoj, la celo de la aventuroj en fabeloj; la plumoj de
      la <tld/>obirdo kaj la <tld/>obirdo mem posedas magian
      forton:
      <ekz>
        <nom>Djagilev</nom> kaj komponisto <nom>Igor Stravinskij</nom> kombinis siajn
        talentojn por konduki rusan folkloron al vivo en
        <ctl>La <tld lit="F"/>obirdo</ctl> kaj
        &leftquot;<nom>Petru&scirc;ka</nom>&rightquot;
        <fnt>
          <vrk><url ref="http://eo.wikipedia.org/wiki/Historio_de_baleto#Rusio">Historio de baleto</url></vrk>. <bib>Viki</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Pale&hcirc;</nom> estas la hejmlando de fabela <tld/>obirdo, kiu pavas sur la blazono kaj
        flago de la urbo, pentrita <klr>[&#8230;]</klr> orkolore sur la nigra fono
        <fnt>
          <vrk><url ref="https://www.ural-sib.org/eo/la-sepa-tago-de-ret-ekskurso-al-palehx-vespera-koncerto-de-la-muzik-bando-ekvinokso">Ekskurso al Pale&hcirc;</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">ocell de foc</trd>
    <trd lng="de">Feuervogel</trd>
    <trd lng="en">Firebird</trd>
    <trd lng="fr">oiseau de feu</trd>
    <trd lng="pl">&zdot;ar-ptak</trd>
    <trd lng="ru">&c_zh;&c_a;&c_r;-&c_p;&c_t;&c_i;&c_c;&c_a;</trd>
  </snc>
  <snc mrk="fajr.0obirdo.fenikso">
    <ref tip="dif" cel="feniks.0o.MIT">fenikso<sncref/></ref>
    <trd lng="ca">f&egrave;nix</trd>
    <trd lng="id">funiks</trd>
  </snc>
  <trd lng="cs">plame&ncaron;&aacute;k</trd>
  <trd lng="sk">plameniak</trd>
</drv>

<drv mrk="fajr.0ostango">
  <kap><tld/>ostango</kap>
  <snc mrk="fajr.0ostango.pikilo">
    <dif>
      Pikilo por eksciti <tld/>on:
      <ekz>
        mia edzino ekkoleras kaj batas min per <tld/>ostango
        <fnt>
          <vrk><url ref="http://literaturo.org/HARLOW-Don/Esperanto/Literaturo/Revuoj/epc/epc6106/afanti.html">Rakontoj pri Afanti</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        per la <tld/>ostango li malfermis la glitpordeton kaj bruligis truon en la fundo
        <fnt>
          <vrk><url ref="https://www.oocities.org/athens/Acropolis/4675/profitulo.html">La profitulo - Nescio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_ch;&c_a;&c_r;&c_g;&c_a;</trd>
  <trd lng="ca">atiador</trd>
  <trd lng="cs">pohrab&aacute;&ccaron;</trd>
  <trd lng="de">Sch&uuml;rhaken</trd>
  <trd lng="en">poker</trd>
  <trd lng="fr">tisonnier</trd>
  <trdgrp lng="hu">
      <trd>piszkafa</trd>,
      <trd>piszkavas</trd>
    </trdgrp>
  <trd lng="nl">pook</trd>
  <trd lng="pl">pogrzebacz</trd>
  <trd lng="ru">&c_k;&c_o;&c_ch;&c_je;&c_r;&c_g;&c_a;</trd>
  <trd lng="sk">kut&aacute;&ccaron;</trd>
</drv>

<drv mrk="fajr.0oprenilo">
  <kap><tld/>oprenilo</kap>
  <snc mrk="fajr.0oprenilo.dubraka">
    <dif>
      Dubraka ilo por preni brulantan materion:
      <ekz>
        la <tld/>o&scirc;ovelilo kaj la <tld/>oprenilo
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        la mastrino kriis kaj penis frapi &gcirc;in per la <tld/>oprenilo
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
      <trd>molls</trd>,
      <trd>tenalles</trd>
    </trdgrp>
  <trd lng="cs">kle&scaron;t&ecaron; na &zcaron;hav&eacute; d&rcaron;evo v krbu</trd>
  <trd lng="de">Feuerzange</trd>
  <trd lng="en">fire <ind>tongs</ind></trd>
  <trd lng="fr">pince &agrave; feu</trd>
  <trd lng="hu">t&udblac;zfog&oacute;</trd>
  <trd lng="nl">vuurtang</trd>
  <trd lng="pl">szczypce do w&eogonek;gli</trd>
  <trd lng="ru">&c_shch;&c_i;&c_p;&c_c;&c_y; &c_d;&c_l;&c_ja;
      &c_u;&c_g;&c_l;&c_je;&c_j;</trd>
  <trd lng="sk">pahrebov&yacute; &scaron;tipec</trd>
  <trd lng="tr">ma&scedil;a</trd>
</drv>

<drv mrk="fajr.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="fajr.0ejo.loko">
    <dif>
      Loko, en kiu <tld/>o brulas:
      <ekz>
        oni povis flarsenti, kiaj man&gcirc;a&jcirc;oj estas pretigitaj sur &ccirc;iu <tld/>ejo en la urbo
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tisto de porkoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> staris plej proksime de la <tld/>ejo
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i sidis senmove kontra&ubreve; la estingi&gcirc;anta <tld/>ejo kaj meditis
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_ch;&c_a;&c_g;</trd>,
    <trd>&c_g;&c_a;&c_r;&c_n;&c_o;</trd>
  </trdgrp>
  <trd lng="br">oaled</trd>
  <trdgrp lng="ca">
    <trd>fogar</trd>,
    <trd>llar</trd>,
    <trd>xemeneia</trd>
  </trdgrp>
  <trdgrp lng="cs">
  <trd>m&iacute;sto kde se top&iacute;</trd>,
  <trd>ohni&scaron;t&ecaron;</trd>
  </trdgrp>
  <trd lng="de">Feuerstelle</trd>
  <trdgrp lng="en">
    <trd>fireplace</trd>,
    <trd>hearth</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>chemin&eacute;e<klr tip="ind"> (foyer)</klr></trd>,
    <trd>foyer<klr tip="ind"> (chemin&eacute;e)</klr></trd>
  </trdgrp>
<trd lng="hu">t&udblac;zhely</trd>
<trd lng="nl">haard</trd>
<trd lng="pl">palenisko</trd>
<trd lng="ru">&c_o;&c_ch;&c_a;&c_g;</trd>
<trd lng="sk">ohnisko</trd>
<trd lng="sv">eldstad</trd>
<trd lng="tr">&scedil;&ouml;mine</trd>
</drv>

<drv mrk="fajr.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="fajr.0ujo.parto">
    <dif>
      Parto de forno a&ubreve; kameno destinita por enteni la
      <tld/>on:
      <ekz>
        la brulofero restu sur la <tld/>ujo sur la altaro dum tuta nokto
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 6:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        preni <tld/>on de la <tld/>ujo
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 30:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        vespere &ccirc;e la <tld/>ujo oni rakontis fabelojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fabelo pri iu, kiu migris por ekkoni timon</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ne havis liton, sed devis ku&scirc;i&gcirc;i apud la <tld/>ujo en la cindro
        <fnt>
          <bib>ElektFab</bib>
          <lok>Cindrulino</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_ch;&c_a;&c_g;</trd>,
    <trd>&c_g;&c_a;&c_r;&c_n;&c_o;</trd>
  </trdgrp>
  <trd lng="br">oaled</trd>
  <trd lng="ca">fogar</trd>
  <trdgrp lng="cs">
    <trd>kov&aacute;&rcaron;.v&yacute;he&ncaron;</trd>,
    <trd>ohni&scaron;t&ecaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Feuermulde</trd>,
    <trd>Ofenloch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>firebox</trd>,
    <trd>brazier</trd>,
    <trd>grate</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>foyer<klr tip="ind"> (&acirc;tre)</klr></trd>,
    <trd>&acirc;tre</trd>
  </trdgrp>
  <trd lng="hu">t&udblac;zt&eacute;r</trd>
  <trd lng="pl">palenisko</trd>
  <trd lng="ru">&c_o;&c_ch;&c_a;&c_g;</trd>
  <trdgrp lng="sk">
    <trd>kozub</trd>,
    <trd>krb</trd>
  </trdgrp>
</drv>

<drv mrk="fajr.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="fajr.0ajxo.PIR">
    <dif>
      Arta <tld/>o, konsistanta el raketoj kaj
      petardoj, kiujn oni ekbruligas en festoj:
      <ekz>
        li a&ccirc;etis raketojn, krakpizojn kaj plej diversajn <tld/>a&jcirc;ojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Fluganta kofro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[iliaj]</klr> flora&jcirc;oj brilis en la klara sunlumo kvaza&ubreve; vera multkolora
        <tld/>a&jcirc;o
        <fnt>
          <bib>Fab3</bib>
          <lok>Folio el la &ccirc;ielo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>a&jcirc;isto</ind>;
        <trd lng="be">&c_p;&c_ib;&c_r;&c_a;&c_t;&c_e;&c_h;&c_n;&c_ib;&c_k;</trd>
        <trd lng="ca">pirot&egrave;cnic</trd>
        <trd lng="de">Feuerwerker</trd>
        <trd lng="fr">artificier<klr tip="ind">
        (pyrotechnicien)</klr></trd>
        <trd lng="hu">t&udblac;zszer&eacute;sz</trd>
        <trd lng="nl">vuurwerkmaker</trd>
        <trd lng="ru">&c_p;&c_i;&c_r;&c_o;&c_t;&c_je;&c_h;&c_n;&c_i;&c_k;</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la norda brilo flamis super la glacimontoj de la nordo, tio estis <tld/>a&jcirc;o,
        kiun neniu povus imiti
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fajr.art0ajxo">artfajra&jcirc;o</ref>,
      <ref cel="pirote.0ajxo">piroteknika&jcirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_je;&c_r;&c_v;&c_e;&c_r;&c_k;</trd>
  <trd lng="br">tan-arvest</trd>
  <trd lng="ca">focs d'artifici</trd>
  <trd lng="de">Feuerwerk</trd>
  <trd lng="en">fireworks</trd>
  <trd lng="es">fuegos artificiales</trd>
  <trd lng="fr">feu d'artifice</trd>
  <trd lng="hu">t&udblac;zij&aacute;t&eacute;k</trd>
  <trd lng="id"><ind>kembang</ind> api</trd>
  <trd lng="ja">&#33457;&#28779;
  [<ind>&#12399;&#12394;&#12403;</ind>]</trd>
  <trd lng="nl">vuurwerk</trd>
  <trdgrp lng="pl">
    <trd>fajerwerk</trd>,
    <trd>sztuczne <ind>ognie</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_je;&c_j;&c_je;&c_r;&c_v;&c_je;&c_r;&c_k;</trd>
  <trd lng="tr">havai fi&scedil;ek</trd>
</drv>

<drv mrk="fajr.art0ajxo">
  <kap>art<tld/>a&jcirc;o</kap>
  <snc mrk="fajr.art0ajxo.PIR">
    <dif>
      <ref tip="dif" cel="fajr.0ajxo">Fajra&jcirc;o</ref>:
      <ekz>
        de sur la apuda monto oni pafu raketojn kaj aliajn art<tld/>a&jcirc;ojn por ne dama&gcirc;i
        en la riveron
        <fnt>
          <bib>Monato</bib>
          <aut>Karlo Juh&aacute;sz</aut>
          <vrk><url ref="https://www.monato.be/2006/009070.php">&Scirc;tormo post &scirc;tormo</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        en majo kaj junio okazas art<tld/>a&jcirc;a konkurso
        <fnt>
          <bib>Monato</bib>
          <aut>Miroslav Hru&scaron;ka</aut>
          <vrk><url ref="https://www.monato.be/2013/011042.php">Brno</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_je;&c_r;&c_v;&c_e;&c_r;&c_k;</trd>
  <trd lng="br">tan-arvest</trd>
  <trd lng="ca">focs d'artifici</trd>
  <trd lng="de">Feuerwerk</trd>
  <trd lng="en">fireworks</trd>
  <trd lng="es">fuegos artificiales</trd>
  <trd lng="fr">feu d'artifice</trd>
  <trd lng="hu">t&udblac;zij&aacute;t&eacute;k</trd>
  <trd lng="id"><ind>kembang</ind> api</trd>
  <trd lng="nl">vuurwerk</trd>
  <trdgrp lng="pl">
    <trd>fajerwerk</trd>,
    <trd>sztuczne <ind>ognie</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_je;&c_j;&c_je;&c_r;&c_v;&c_je;&c_r;&c_k;</trd>
</drv>

<drv mrk="fajr.bengala0o">
  <kap>bengala <tld/>o</kap>
  <snc mrk="fajr.bengala0o.PIR">
    <dif>
      <ref tip="super" cel="fajr.0ajxo.PIR">Fajra&jcirc;o</ref>
      en formo de bastoneto, kiu forbrulas kun tre hela, ofte kolora lumo kaj fumo:
      <ekz>
        en la spacego eksplodus belega bengala <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        poste per bengala <tld/>o /
        ludis li en dom' el pajlo
        <fnt>
          <aut>R. Rossetti</aut>,
          <vrk><url ref="http://literaturo.org/HARLOW-Don/Esperanto/Literaturo/Revuoj/nlr/nlr11/infanrimajoj.html">
          Infanrima&jcirc;oj</url></vrk>,
          <lok>La Nica Literatura Revuo, 1955-09 &gcirc;is 10 (1/1), p.
          19a</lok>
        </fnt>;
      </ekz>
      <ekz>
        bengal<tld/>a&jcirc;o &jcirc;etita apud la itala golulo
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.be/2011/010198.php"
              >Futbalejo a&ubreve; &ccirc;u batalejo?</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Bengalisches Feuer</trd>
  <trd lng="fr">feu de bengale</trd>
</drv>

<drv mrk="fajr.ligno0o">
  <kap>ligno<tld/>o</kap>
  <snc mrk="fajr.ligno0o.KOMUNE">
    <dif>
      <tld lit="F"/>o farita el ardantaj &scirc;tipoj,
      lignopecoj, bran&ccirc;oj kaj similaj:
      <ekz>
        gardistoj, sidantaj la tutan tagon &ccirc;e la ligno<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Lena Karpunina</aut>
          <vrk><url ref="https://www.monato.be/2004/008350.html">Tiu tago</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili vidis sub si multajn ligno<tld/>ojn
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Vidanta Mano</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">foguera</trd>
  <trdgrp lng="cs">
  <trd>hranice</trd>,
  <trd>ohe&ncaron;</trd>,
  <trd>t&aacute;bor&aacute;k</trd>
  </trdgrp>
  <trd lng="de">Holzfeuer</trd>
  <trdgrp lng="es">
    <trd>hoguera</trd>,
    <trd>fogata</trd>
  </trdgrp>
  <trd lng="id"><ind>api</ind> unggun</trd>
  <trdgrp lng="sk">
    <trd>hranica</trd>,
    <trd>ohe&ncaron;</trd>,
    <trd>t&aacute;bor&aacute;k</trd>
  </trdgrp>
</drv>

