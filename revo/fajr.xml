<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fajr.xml,v 1.41 2020/01/09 00:10:31 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fajr</rad>/o
</kap>

<drv mrk="fajr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fajr.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Fenomeno konsistanta en brulkonsumado de diversaj
      substancoj, kun produktado de lumo kaj varmo:
      <ekz>
        fari <klr>(ekbruligi)</klr> <tld/>on
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz><ekz>
        bloveksciti la <tld/>on
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;irka&ubreve;a&jcirc;o <klr>[&#8230;]</klr> dezertigita per <tld/>o kaj glavo
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz><ekz>
        tro akra <tld/>o estas sen da&ubreve;ro
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flam.0o">flamo</ref>,
      <ref cel="ard">ardo</ref>
    </refgrp>
    <trd lng="br">tan</trd>
    <trd lng="ca">foc</trd>
    <trd lng="en">fire</trd>
    <trd lng="ja">&#28779; [<ind>&#12402;</ind>]</trd>
    <trd lng="pl">ogie&nacute;</trd>
    <trdgrp lng="pt">
      <trd>fogo</trd>,
      <trd>lume</trd>
    </trdgrp>
    <trd lng="tp">seli</trd>
    <trd lng="vo">fil</trd>
  </snc>
  <snc mrk="fajr.0o.fervoro">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Entuziasmo, granda fervoro, ardeco:
      <ekz>
        en la <tld/>o de mia kolero
        <fnt><bib>MT</bib>, <lok>&Jeh; 38:19</lok></fnt>;
      </ekz><ekz>
        diskuti kun <tld/>o;
      </ekz>
      <ekz>
        nun, &dash; ekkriis li kun <tld/>o, &dash; nun mi havas celon en la vivo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i reblovas en li la <tld/>on de l&#8217; ven&gcirc;emo
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ard.0o">ardo</ref>,
      <ref cel="ekzalt">ekzalteco</ref>,
      <ref cel="entuzi.0o">entuziasmo</ref>,
      <ref cel="febr.0o.fervoro">febro<sncref/></ref>,
      <ref cel="fervor.0o">fervoro</ref>,
      <ref cel="flam.0o.fervoro">flamo<sncref/></ref>,
      <ref cel="pasi.0o">pasio</ref>,
      <ref cel="varm.0o">varmo</ref>,
      <ref cel="verv.0o">vervo</ref>,
      <ref cel="vigl">vigleco</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="incit.0i">inciti</ref>.
    </refgrp>
    <trdgrp lng="br">
      <trd>entan</trd>,
      <trd>birvidigezh</trd>,
      <trd>gred</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>entusiasme</trd>,
      <trd>passi&oacute;</trd>,
      <trd>enardiment</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>ardor</trd>,
      <trd>enthusiasm</trd>,
      <trd>fervor</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>ogie&nacute;</trd>,
      <trd>zapa&lstroke;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>fogo</trd>,
      <trd>entusiasmo</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_a;&c_g;&c_o;&c_n;&c_mol;</trd>
  <trd lng="bg">&c_o;&c_g;&c_malmol;&c_n;</trd>
  <trd lng="ca">foc</trd>
  <trd lng="cs">ohe&ncaron;</trd>
  <trd lng="de">Feuer</trd>
  <trd lng="es">fuego</trd>
  <trd lng="fr">feu</trd>
  <trd lng="hu">t&udblac;z</trd>
  <trd lng="id">api</trd>
  <trd lng="nl">vuur</trd>
  <trd lng="ru">&c_o;&c_g;&c_o;&c_n;&c_mol;</trd>
  <trd lng="sk">ohe&ncaron;</trd>
  <trd lng="sv">eld</trd>
  <trd lng="tr">ate&scedil;</trd>
  <trd lng="zh">&#28779;<klr> [hu&#466;]</klr></trd>
</drv>

<drv mrk="fajr.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      De <tld/>o, simila al <tld/>o, havanta econ de <tld/>o:
      <ekz>
        <tld/>a flamo
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 15:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; la trono brulis sep <tld/>aj tor&ccirc;oj
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 4:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a kolero
        <fnt>
          <bib>NT</bib>
          <lok>Hebreoj 10:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>aj kisoj
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;i ne rimarkis liajn <tld/>ajn mallumajn okulojn
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        nigraj <tld/>aj &ccirc;evaloj blekis en la stalo
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_o;&c_g;&c_n;&c_je;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_g;&c_n;&c_ib;&c_s;&c_t;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_y;&c_m;&c_ja;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>entanet</trd>,
  </trdgrp>
  <trdgrp lng="ca">
    <trd>igni</trd>,
    <trd>de foc</trd>,
    <trd>ardent</trd>,
    <trd>incandescent</trd>
  </trdgrp>
  <trd lng="cs">ohniv&yacute;</trd>
  <trdgrp lng="en">
    <trd>burning</trd>,
    <trd>fiery</trd>
  </trdgrp>
  <trd lng="es">fogoso</trd>
  <trdgrp lng="fr">
    <trd>de <ind>feu</ind></trd>,
    <trd>fougueux</trd>,
    <trd>ign&eacute;</trd>,
    <trd>enflamm&eacute;<klr tip="ind">
    (passionn&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="hu">t&uuml;zes</trd>
  <trd lng="id">[<ind>api</ind>] berapi</trd>
  <trd lng="nl">vurig</trd>
  <trd lng="pl">ognisty</trd>
  <trd lng="ru">&c_o;&c_g;&c_n;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">oh&ncaron;ov&yacute;</trd>
  <trd lng="tr">ate&scedil;li</trd>
</drv>

<drv mrk="fajr.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      En <tld/>a maniero:
      <ekz>
        <tld/>e ru&gcirc;aj floroj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>e ami, paroli;
      </ekz>
      <ekz>
        <tld/>e brulis liaj &ccirc;irka&ubreve;prenoj / forte batis koro &ccirc;e la koro
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_y;&c_m;&c_ja;&c_n;&c_a;</trd>
  <trdgrp lng="br">
      <trd>gant <ind>entan</ind></trd>,
      <trd>gant <ind>birvidigezh</ind></trd>,
      <trd>gant <ind>gred</ind></trd>
    </trdgrp>
  <trdgrp lng="ca">
      <trd>apassionadament</trd>,
      <trd>ardentment</trd>
    </trdgrp>
  <trd lng="cs">ohniv&ecaron;</trd>
  <trd lng="de">feurig</trd>
  <trdgrp lng="fr">
      <trd>avec <ind>feu</ind></trd>,
      <trd>avec <ind>fougue</ind></trd>,
      <trd>fougueusement</trd>,
      <trd>passionn&eacute;ment</trd>
    </trdgrp>
  <trd lng="hu">t&uuml;zesen</trd>
  <trd lng="nl">vurig</trd>
  <trd lng="pl">ogni&sacute;cie</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_m;&c_je;&c_n;&c_n;&c_o;</trd>
  <trd lng="sk">ohnivo</trd>
  <trdgrp lng="tr">
      <trd>ate&scedil;le</trd>,
      <trd>tutkuyla</trd>
    </trdgrp>
</drv>

<drv mrk="fajr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="fajr.0i.KOMUNE">
    <dif>
      Bruli kun <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_e;&c_c;&c_mol;</trd>
  <trdgrp lng="br">
      <trd>flamma&ntilde;</trd>,
      <trd>flammenni&ntilde;</trd>,
      <trd>flammina&ntilde;</trd>
    </trdgrp>
  <trd lng="ca">cremar</trd>
  <trdgrp lng="cs">
  <trd>fajrovat</trd>,
  <trd>ho&rcaron;et</trd>,
  <trd>pl&aacute;t</trd>
  </trdgrp>
  <trdgrp lng="en">
      <trd>burn</trd>,
      <trd>flame</trd>
    </trdgrp>
  <trd lng="es">arder</trd>
  <trd lng="fr">flamber</trd>
  <trdgrp lng="hu">
      <trd>&eacute;g</trd>,
      <trd>t&uuml;zel<klr tip="ind"> (&eacute;g)</klr></trd>
    </trdgrp>
  <trdgrp lng="nl">
      <trd>aansteken</trd>,
      <trd>flamberen</trd>
    </trdgrp>
  <trdgrp lng="pl">
      <trd>p&lstroke;on&aogonek;&cacute;</trd>,
      <trd>gorze&cacute;<klr> (przest.)</klr></trd>,
      <trd>pali&cacute; si&eogonek;</trd>
    </trdgrp>
  <trdgrp lng="ru">
      <trd>&c_g;&c_o;&c_r;&c_je;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_l;&c_a;&c_m;&c_je;&c_n;&c_je;&c_t;&c_mol;</trd>
    </trdgrp>
  <trd lng="sk">horie&tcaron;</trd>
  <trd lng="tr">alev</trd>
</drv>

<drv mrk="fajr.0ero">
  <kap><ofc>*</ofc><tld/>ero</kap>
  <snc mrk="fajr.0ero.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Arda parteto el&jcirc;etita de brulanta korpo, produktata de
      du kunfrotaj malmolaj korpoj, a&ubreve; eligata de <ref
      cel="elektr.0umi">elektrumita</ref> korpo
      <ekz>
        unu <tld/>ero estas sufi&ccirc;a, por eksplodigi pulvon
        <fnt><bib>F</bib>, <lok>&FE; 41</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="elektr.0afajrero">elektra <tld/>ero</ref>,
    </refgrp>
    <adm>
      Vidu administran noton sub elektr pri elektrumi. Cetere
      "elektra fajrero"
      pli bone kusxus &ccirc;i tie ol sub "elektr".
      <aut>MB</aut>
    </adm>
  </snc>
  <refgrp tip="sin">
    <ref cel="spark.0o">sparko</ref>
  </refgrp>
  <trd lng="be">&c_ib;&c_s;&c_k;&c_r;&c_a;</trd>
  <trdgrp lng="br">
    <trd>elf</trd>,
    <trd>fulenn<klr tip="ind"> (tan)</klr></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>espurna</trd>,
    <trd>guspira</trd>
  </trdgrp>
  <trd lng="de">Funke</trd>
  <trd lng="en">spark</trd>
  <trd lng="es">chispa</trd>
  <trd lng="fr">&eacute;tincelle</trd>
  <trd lng="hu">szikra</trd>
  <trd lng="ja">&#28779;&#33457;
  [<ind>&#12402;&#12400;&#12394;</ind>]</trd>
  <trd lng="nl">vonk</trd>
  <trdgrp lng="pl">
    <trd>iskra</trd>,
    <trd>skra</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_s;&c_k;&c_r;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#28779;&#26143;<klr> [hu&#466;x&#299;ng]</klr></trd>,
    <trd>&#28779;&#26143;&#20799;</trd>,
    <trd><klr>tr. </klr>&#28779;&#26143;&#20818;<klr>
        [hu&#466;x&#299;ngr]</klr></trd>,
    <trd>&#26143;&#28779;<klr> [x&#299;nghu&#466;]</klr></trd>,
    <trd>&#28779;&#33457;<klr> [hu&#466;hu&#257;]</klr></trd>
  </trdgrp>
  <trd lng="tr">k&inodot;v&inodot;lc&inodot;m</trd>
  <trd lng="sk">iskra</trd>
<trd lng="cs">jiskra z ohn&ecaron;</trd>
</drv>

<drv mrk="fajr.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fajr.0igi.KOMUNE">
    <ref tip="dif" cel="brul.ek0igi">ekbruligi</ref>
    <ekz>
      certe &ccirc;io, kion oni rakontis estis nur fabelo, kiu
      <tld/>igis la imagojn
      <fnt>
      <aut>J. Modest</aut>,
      <vrk>La ikono</vrk>,
      <bib>Monato</bib>,
      <lok>2001:12, p. 23a</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd
    lng="be">&c_z;&c_a;&c_p;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="br">
      <trd>tana&ntilde;</trd>,
      <trd>entana&ntilde;</trd>,
      <trd>c'hwezha&ntilde;<klr> (an tan)</klr></trd>
    </trdgrp>
  <trdgrp lng="ca">
      <trd>encendre</trd>,
      <trd>inflamar</trd>,
      <trd>calar foc</trd>,
      <trd>incendiar</trd>
    </trdgrp>
  <trd lng="cs">zap&aacute;lit</trd>
  <trdgrp lng="de">
      <trd>anz&uuml;nden</trd>,
      <trd>entz&uuml;nden</trd>,
      <trd>entflammen</trd>,
      <trd>entfachen</trd>
    </trdgrp>
  <trd lng="es">darle fuego a</trd>
  <trdgrp lng="fr">
      <trd>allumer <klr tip="ind">(mettre le feu
      &agrave;)</klr></trd>,
      <trd>mettre le <ind>feu</ind> (&agrave;)</trd>
    </trdgrp>
  <trdgrp lng="hu">
      <trd>l&aacute;ngra lobbant</trd>,
      <trd>meggy&uacute;jt</trd>
    </trdgrp>
  <trd lng="nl">aansteken</trd>
  <trdgrp lng="pl">
      <trd>rozpala&cacute;</trd>,
      <trd>zapala&cacute;</trd>
    </trdgrp>
  <trdgrp lng="pt">
      <trd>atear <klr>(fogo)</klr></trd>,
      <trd>acender <klr>(o fogo)</klr></trd>,
      <trd>tacar <klr>(fogo)</klr></trd>
    </trdgrp>
  <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_zh;&c_i;&c_g;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_zh;&c_je;&c_ch;&c_mol;</trd>
    </trdgrp>
  <trd lng="sk">zap&aacute;li&tcaron;</trd>
  <trd lng="tr">ate&scedil; yakmak</trd>
</drv>

<drv mrk="fajr.0ilo">
  <kap><tld/>ilo<fnt>Z</fnt></kap>
  <snc mrk="fajr.0ilo.KOMUNE">
    <dif>
      Malgranda ilo por produkti <tld/>erojn per frotado de
