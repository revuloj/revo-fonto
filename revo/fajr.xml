  <trd lng="sk">pahrebov&yacute; &scaron;tipec</trd>
  <trd lng="tr">ma&scedil;a</trd>
</drv>

<drv mrk="fajr.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Loko, en kiu <tld/>o brulas.
    </dif>
  </snc>
<trdgrp lng="be">
    <trd>&c_a;&c_ch;&c_a;&c_g;</trd>,
    <trd>&c_g;&c_a;&c_r;&c_n;&c_o;</trd>
  </trdgrp>
<trd lng="br">oaled</trd>
<trdgrp lng="ca">
    <trd>fogar</trd>,
    <trd>llar</trd>,
    <trd>xemeneia</trd>
  </trdgrp>
<trdgrp lng="cs">
<trd>m&iacute;sto kde se top&iacute;</trd>,
<trd>ohni&scaron;t&ecaron;</trd>
</trdgrp>
<trd lng="de">Feuerstelle</trd>
<trdgrp lng="en">
