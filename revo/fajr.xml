<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>fajr</rad>/o
</kap>

<drv mrk="fajr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc mrk="fajr.0o.FIZ">
    <dif>
      Fenomeno konsistanta en brulkonsumado de diversaj substancoj, kun
      produktado de lumo kaj varmo:
      <ekz>
        fari <klr>(ekbruligi)</klr> <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        bloveksciti la <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        dezertigi landon per <tld/>o kaj glavo
        <fnt>Z</fnt>;
      </ekz>
      <ekz>
        tro akra <tld/>o estas sen da&ubreve;ro<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flam.0o">flamo</ref>,
      <ref cel="ard">ardo</ref>.
    </refgrp>
    <trd lng="br">tan</trd>
    <trd lng="en">fire</trd>
    <trd lng="ja">&#28779; [<ind>&#12402;</ind>]</trd>
    <trdgrp lng="pt">
      <trd>fogo</trd>,
      <trd>lume</trd>
    </trdgrp>
    <trd lng="tp">seli</trd>
    <trd lng="vo">fil</trd>
  </snc>
  <snc mrk="fajr.0o.fervoro">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Entuziasmo, granda fervoro, ardeco:
      <ekz>
        en la <tld/>o de mia kolero
        <fnt><bib>MT</bib>, <lok>Je&hcirc;ezkel 38, 19</lok></fnt>;
      </ekz>
      <ekz>
        diskuti kun <tld/>o;
      </ekz>
      <ekz>
        reblovi en iu la <tld/>on de la ven&gcirc;emo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ard.0o">ardo</ref>,
      <ref cel="ekzalt">ekzalteco</ref>,
      <ref cel="entuzi.0o">entuziasmo</ref>,
      <ref cel="febr.0o.fervoro">febro <sncref/></ref>,
      <ref cel="fervor.0o">fervoro</ref>,
      <ref cel="flam.0o.fervoro">flamo <sncref/></ref>,
      <ref cel="pasi.0o">pasio</ref>,
      <ref cel="varm.0o">varmo</ref>,
      <ref cel="verv.0o">vervo</ref>,
      <ref cel="vigl">vigleco</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="incit.0i">inciti</ref>.
    </refgrp>
    <trdgrp lng="br">
      <trd>entan</trd>,
      <trd>birvidigezh</trd>,
      <trd>gred</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>ardor</trd>,
      <trd>enthusiasm</trd>,
      <trd>fervor</trd>
    </trdgrp>
  <trdgrp lng="pt">
    <trd>fogo</trd>,
    <trd>entusiasmo</trd>
  </trdgrp>
  </snc>
  <trd lng="be">&c_a;&c_g;&c_o;&c_n;&c_mol;</trd>
  <trd lng="bg">&c_o;&c_g;&c_malmol;&c_n;</trd>
  <trd lng="cs">ohe&ncaron;</trd>
  <trd lng="de">Feuer</trd>
  <trd lng="fr">feu</trd>
  <trd lng="hu">t&udblac;z</trd>
  <trd lng="nl">vuur</trd>
  <trd lng="ru">&c_o;&c_g;&c_o;&c_n;&c_mol;</trd>
  <trd lng="sv">eld</trd>
  <trd lng="zh">&#28779;<klr> [hu&#466;]</klr></trd>
  <trd lng="tr">ate&scedil;</trd>
</drv>

<drv mrk="fajr.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      De <tld/>o, simila al <tld/>o, havanta econ de <tld/>o:
      <ekz>
        la interna&jcirc;o de la tero konsistas el <tld/>a fandita
        maso;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a ardo, brilo<fnt>B</fnt>, deziro<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>aj okuloj, rigardoj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_o;&c_g;&c_n;&c_je;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_g;&c_n;&c_ib;&c_s;&c_t;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_y;&c_m;&c_ja;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>entanet</trd>,
  </trdgrp>
  <trdgrp lng="en">
    <trd>burning</trd>,
    <trd>fiery</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>de <ind>feu</ind></trd>,
    <trd>fougueux</trd>,
    <trd>ign&eacute;</trd>,
    <trd>enflamm&eacute;<klr tip="ind"> (passionn&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="hu">t&uuml;zes</trd>
  <trd lng="nl">vurig</trd>
  <trd lng="ru">&c_o;&c_g;&c_n;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  <trd lng="tr">ate&scedil;li</trd>
</drv>

<drv mrk="fajr.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      En <tld/>a maniero:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>e ami, paroli.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_y;&c_m;&c_ja;&c_n;&c_a;</trd>
  <trdgrp lng="br">
    <trd>gant <ind>entan</ind></trd>,
    <trd>gant <ind>birvidigezh</ind></trd>,
    <trd>gant <ind>gred</ind></trd>
  </trdgrp>
  <trd lng="de">feurig</trd>
  <trdgrp lng="fr">
    <trd>avec <ind>feu</ind></trd>,
    <trd>avec <ind>fougue</ind></trd>,
    <trd>fougueusement</trd>,
    <trd>passionn&eacute;ment</trd>
  </trdgrp>
  <trd lng="hu">t&uuml;zesen</trd>
  <trd lng="nl">vurig</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_m;&c_je;&c_n;&c_n;&c_o;</trd>
  <trdgrp lng="tr">
    <trd>ate&scedil;le</trd>,
    <trd>tutkuyla</trd>
  </trdgrp>
</drv>

<drv mrk="fajr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Bruli kun <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_e;&c_c;&c_mol;</trd>
  <trdgrp lng="br">
    <trd>flamma&ntilde;</trd>,
    <trd>flammenni&ntilde;</trd>,
    <trd>flammina&ntilde;</trd>
  </trdgrp>  <trdgrp lng="en">
    <trd>burn</trd>,
    <trd>flame</trd>
  </trdgrp>
  <trd lng="fr">flamber</trd>
  <trdgrp lng="hu">
    <trd>&eacute;g</trd>,
    <trd>t&uuml;zel<klr tip="ind"> (&eacute;g)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>aansteken</trd>,
    <trd>flamberen</trd>
  </trdgrp>
  <trd lng="tr">alev</trd>
</drv>

<drv mrk="fajr.0ero">
  <kap><ofc>*</ofc><tld/>ero</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Arda parteto el&jcirc;etita de brulanta korpo, produktata de du
      kunfrotaj
      malmolaj korpoj, a&ubreve; eligata de <ref
      cel="elektr.0umi">elektrumita</ref> korpo
      <ekz>unu <tld/>ero estas sufi&ccirc;a, por eksplodigi pulvon
      <fnt><bib>F</bib>, <lok>&FE; 41</lok></fnt>.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="elektr.0afajrero">elektra <tld/>ero</ref>,
    </refgrp>
    <adm>
      Vidu administran noton sub elektr pri elektrumi. Cetere "elektra
      fajrero"
      pli bone kusxus &ccirc;i tie ol sub "elektr".
      <aut>MB</aut>
    </adm>
  </snc>
  <refgrp tip="sin">
    <ref cel="spark.0o">sparko</ref>
  </refgrp>
  <trd lng="be">&c_ib;&c_s;&c_k;&c_r;&c_a;</trd>
  <trdgrp lng="br">
    <trd>elf</trd>,
    <trd>fulenn<klr tip="ind"> (tan)</klr></trd>
  </trdgrp>
  <trd lng="de">Funke</trd>
  <trd lng="en">spark</trd>
  <trd lng="fr">&eacute;tincelle</trd>
  <trd lng="hu">szikra</trd>
  <trd lng="ja">&#28779;&#33457; [<ind>&#12402;&#12400;&#12394;</ind>]</trd>
  <trd lng="nl">vonk</trd>
  <trd lng="ru">&c_i;&c_s;&c_k;&c_r;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#28779;&#26143;<klr> [hu&#466;x&#299;ng]</klr></trd>,
    <trd>&#28779;&#26143;&#20799;</trd>,
    <trd><klr>tr. </klr>&#28779;&#26143;&#20818;<klr>
[hu&#466;x&#299;ngr]</klr></trd>,
    <trd>&#26143;&#28779;<klr> [x&#299;nghu&#466;]</klr></trd>,
    <trd>&#28779;&#33457;<klr> [hu&#466;hu&#257;]</klr></trd>
  </trdgrp>
  <trd lng="tr">k&inodot;v&inodot;lc&inodot;m</trd>
</drv>

<drv mrk="fajr.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <ref tip="dif" cel="brul.ek0igi">ekbruligi</ref>
    <ekz>
      certe &ccirc;io, kion oni rakontis estis nur fabelo, kiu <tld/>igis la
      imagojn<fnt><aut>Julian Modest</aut>: <vrk>La ikono</vrk>,
      <bib>Monato</bib>, <lok>jaro 2001a, numero 12a, p.
      23a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_p;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="br">
    <trd>tana&ntilde;</trd>,
    <trd>entana&ntilde;</trd>,
    <trd>c'hwezha&ntilde;<klr> (an tan)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>anz&uuml;nden</trd>,
    <trd>entz&uuml;nden</trd>,
    <trd>entflammen</trd>,
    <trd>entfachen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>allumer <klr tip="ind">(mettre le feu &agrave;)</klr></trd>,
    <trd>mettre le <ind>feu</ind> (&agrave;)</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>l&aacute;ngra lobbant</trd>,
    <trd>meggy&uacute;jt</trd>
  </trdgrp>
  <trd lng="nl">aansteken</trd>
  <trdgrp lng="pt">
    <trd>atear <klr>(fogo)</klr></trd>,
    <trd>acender <klr>(o fogo)</klr></trd>,
    <trd>tacar <klr>(fogo)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_zh;&c_i;&c_g;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_zh;&c_je;&c_ch;&c_mol;</trd>
  </trdgrp>
  <trd lng="tr">ate&scedil; yakmak</trd>
</drv>

<drv mrk="fajr.0ilo">
  <kap><tld/>ilo<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Malgranda ilo por produkti <tld/>erojn per frotado de silik&scirc;tono
      kaj
      &scirc;talpeco, a&ubreve; per alia procedo.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_p;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="br">direnn</trd>
  <trd lng="de">Feuerzeug</trd>
  <trd lng="en">lighter</trd>
  <trd lng="fr">briquet</trd>
  <trdgrp lng="hu">
    <trd>t&udblac;zszersz&aacute;m</trd>,
    <trd>&ouml;ngy&uacute;jt&oacute;</trd>
  </trdgrp>
  <trd lng="nl">aansteker</trd>
  <trd lng="pt">isqueiro</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_je;&c_s;&c_a;&c_l;&c_o;</trd>,
    <trd>&c_z;&c_a;&c_zh;&c_i;&c_g;&c_a;&c_l;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="fajr.0ostango">
  <kap><tld/>ostango</kap>
  <snc>
    <dif>
      Pikilo por eksciti <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_ch;&c_a;&c_r;&c_g;&c_a;</trd>
  <trd lng="de">Sch&uuml;rhaken</trd>
  <trd lng="en">poker</trd>
  <trd lng="fr">tisonnier</trd>
  <trdgrp lng="hu">
    <trd>piszkafa</trd>,
    <trd>piszkavas</trd>
  </trdgrp>
  <trd lng="nl">pook</trd>
  <trd lng="ru">&c_k;&c_o;&c_ch;&c_je;&c_r;&c_g;&c_a;</trd>
</drv>

<drv mrk="fajr.0oprenilo">
  <kap><tld/>oprenilo<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Dubraka ilo por preni brulantan materion.
    </dif>
  </snc>
  <trd lng="de">Feuerzange</trd>
  <trd lng="en">fire <ind>tongs</ind></trd>
  <trd lng="fr">pince &agrave; feu</trd>
  <trd lng="hu">t&udblac;zfog&oacute;</trd>
  <trd lng="nl">vuurtang</trd>
  <trd lng="ru">&c_shch;&c_i;&c_p;&c_c;&c_y; &c_d;&c_l;&c_ja;
&c_u;&c_g;&c_l;&c_je;&c_j;</trd>
<trd lng="tr">ma&scedil;a</trd>
</drv>

<drv mrk="fajr.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Loko, en kiu <tld/>o brulas.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_ch;&c_a;&c_g;</trd>,
    <trd>&c_g;&c_a;&c_r;&c_n;&c_o;</trd>
  </trdgrp>
  <trd lng="br">oaled</trd>
  <trd lng="de">Feuerstelle</trd>
  <trdgrp lng="en">
    <trd>fireplace</trd>,
    <trd>hearth</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>chemin&eacute;e<klr tip="ind"> (foyer)</klr></trd>,
    <trd>foyer<klr tip="ind"> (chemin&eacute;e)</klr></trd>
  </trdgrp>
  <trd lng="hu">t&udblac;zhely</trd>
  <trd lng="nl">haard</trd>
  <trd lng="ru">&c_o;&c_ch;&c_a;&c_g;</trd>
  <trd lng="sv">eldstad</trd>
  <trd lng="tr">&scedil;&ouml;mine</trd>
</drv>

<drv mrk="fajr.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <dif>
      Parto de forno a&ubreve; kameno destinita por enteni la <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_ch;&c_a;&c_g;</trd>,
    <trd>&c_g;&c_a;&c_r;&c_n;&c_o;</trd>
  </trdgrp>
  <trd lng="br">oaled</trd>
  <trdgrp lng="en">
    <trd>firebox</trd>,
    <trd>brazier</trd>,
    <trd>grate</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>foyer<klr tip="ind"> (&acirc;tre)</klr></trd>,
    <trd>&acirc;tre</trd>
  </trdgrp>
  <trd lng="hu">t&udblac;zt&eacute;r</trd>
  <trd lng="ru">&c_o;&c_ch;&c_a;&c_g;</trd>
</drv>

<drv mrk="fajr.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Arta <tld/>o, konsistanta el raketoj kaj
      petardoj, kiujn oni ekbruligas en festoj.
    </dif>
    <refgrp tip="sin">
      <ref cel="fajr.art0ajxo">artfajra&jcirc;o</ref>,
      <ref cel="pirote.0ajxo">piroteknika&jcirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_je;&c_r;&c_v;&c_e;&c_r;&c_k;</trd>
  <trd lng="br">tan-arvest</trd>
  <trd lng="de">Feuerwerk</trd>
  <trd lng="en">fireworks</trd>
  <trd lng="fr">feu d'artifice</trd>
  <trd lng="hu">t&udblac;zij&aacute;t&eacute;k</trd>
  <trd lng="ja">&#33457;&#28779; [<ind>&#12399;&#12394;&#12403;</ind>]</trd>
  <trd lng="nl">vuurwerk</trd>
  <trd lng="ru">&c_f;&c_je;&c_j;&c_je;&c_r;&c_v;&c_je;&c_r;&c_k;</trd>
  <trd lng="tr">havai fi&scedil;ek</trd>
</drv>

<drv mrk="fajr.0ajxisto">
  <kap><tld/>a&jcirc;isto</kap>
  <snc>
    <dif>
      Tiu, kiu faras art<tld/>a&jcirc;ojn.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_r;&c_a;&c_t;&c_e;&c_h;&c_n;&c_ib;&c_k;</trd>
  <trd lng="de">Feuerwerker</trd>
  <trd lng="fr">artificier<klr tip="ind"> (pyrotechnicien)</klr></trd>
  <trd lng="hu">t&udblac;zszer&eacute;sz</trd>
  <trd lng="nl">vuurwerkmaker</trd>
  <trd lng="ru">&c_p;&c_i;&c_r;&c_o;&c_t;&c_je;&c_h;&c_n;&c_i;&c_k;</trd>
</drv>

<drv mrk="fajr.art0ajxo">
  <kap>art<tld/>a&jcirc;o</kap>
  <snc>
    <ref tip="dif" cel="fajr.0ajxo">fajra&jcirc;o</ref>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_je;&c_r;&c_v;&c_e;&c_r;&c_k;</trd>
  <trd lng="br">tan-arvest</trd>
  <trd lng="de">Feuerwerk</trd>
  <trd lng="en">fireworks</trd>
  <trd lng="fr">feu d'artifice</trd>
  <trd lng="hu">t&udblac;zij&aacute;t&eacute;k</trd>
  <trd lng="nl">vuurwerk</trd>
  <trd lng="ru">&c_f;&c_je;&c_j;&c_je;&c_r;&c_v;&c_je;&c_r;&c_k;</trd>
</drv>
</art>
