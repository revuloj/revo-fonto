<drv mrk="fajr.0e">
  <kap><tld/>e</kap>
  <snc mrk="fajr.0e.maniero">
    <dif>
      En <tld/>a maniero:
      <ekz>
        <tld/>e ru&gcirc;aj floroj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li parolis <tld/>e kaj deklame kaj faris grandajn gestojn
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        lin interrompis sinjorino Anneto, <tld/>e lin ekkisinte
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>e brulis liaj &ccirc;irka&ubreve;prenoj / forte batis koro &ccirc;e la koro
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj okuloj <tld/>e ru&gcirc;as pro plorado
        <fnt>
          <bib>Ret</bib>
          <lok>Aldono</lok>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_y;&c_m;&c_ja;&c_n;&c_a;</trd>
  <trdgrp lng="br">
      <trd>gant <ind>entan</ind></trd>,
      <trd>gant <ind>birvidigezh</ind></trd>,
      <trd>gant <ind>gred</ind></trd>
    </trdgrp>
  <trdgrp lng="ca">
      <trd>apassionadament</trd>,
      <trd>ardentment</trd>
    </trdgrp>
  <trd lng="cs">ohniv&ecaron;</trd>
  <trd lng="de">feurig</trd>
  <trdgrp lng="en">
    <trd>fierily</trd>,
    <trd>burningly</trd>,
    <trd>with fire</trd>
  </trdgrp>
  <trdgrp lng="fr">
      <trd>avec <ind>feu</ind></trd>,
      <trd>avec <ind>fougue</ind></trd>,
      <trd>fougueusement</trd>,
      <trd>passionn&eacute;ment</trd>
    </trdgrp>
  <trd lng="hu">t&uuml;zesen</trd>
  <trdgrp lng="ja">
    <trd>&#x706B;&#x306E;&#x3088;&#x3046;&#x306B;  [<ind>&#x3072;&#x306E;&#x3088;&#x3046;&#x306B;</ind>]</trd>,
    <trd>&#x71B1;&#x70C8;&#x306B;  [<ind>&#x306D;&#x3064;&#x308C;&#x3064;&#x306B;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">vurig</trd>
  <trd lng="pl">ogni&sacute;cie</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_m;&c_je;&c_n;&c_n;&c_o;</trd>
  <trd lng="sk">ohnivo</trd>
  <trdgrp lng="tr">
      <trd>ate&scedil;le</trd>,
      <trd>tutkuyla</trd>
    </trdgrp>
</drv>

<drv mrk="fajr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="fajr.0i.KOMUNE">
    <dif>
      <ref tip="sin" cel="brul.0i">Bruli</ref> kun a&ubreve; kiel <tld/>o:
      <ekz>
        vespere, la 1-an de julio 1915, la &ccirc;ielo ru&gcirc;e <tld/>is &ccirc;irka&ubreve;
        Varsovio
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>dis<tld/>i</ind>s arbotufoj kaj herbejoj
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en pasia incendio <tld/>as vi
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#x2116;4</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> tuta mondo &scirc;ajnas plen<tld/>e
        <fnt>
          <bib>Rob</bib>
          <lok>Parto II</lok>
        </fnt>;
      </ekz>
      <ekz>
        meti sur<tld/>en la tekru&ccirc;on
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
          <lok>Dua skizo:</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kaldronegoj restis plene pri<tld/>ataj, por ke la &scirc;iprapideco ne malplii&gcirc;u
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 27</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Katenas lin ligiloj de la amo &parsl; Kaj kien ajn li iras, jen la flamo &parsl; Fajranta
        lian bruston sen espero
        <fnt>
       <aut>Manoel BORGES DOS SANTOS</aut>
          <vrk><url ref="&Monato;/2000/006863.php">Poemoj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/04, p. 32</lok>
        </fnt>!
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>in <ind>Flammen</ind> stehen</trd>,
      <trd>brennen</trd>
    </trdgrp>
  </snc>
  <snc mrk="fajr.0i.flagri">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="sin" cel="flagr.0i">Flagri</ref>, <ref cel="bril.0i">brili</ref>
      kiel lumo de <tld/>o:
      <ekz>
        sur <klr>[liaj]</klr> fingroj efektive <tld/>is briloj de juveloj
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj okuloj <tld/>is kiel pupiloj de lupo
        <fnt>
