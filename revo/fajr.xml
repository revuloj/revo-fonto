<drv mrk="fajr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fajr.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Fenomeno konsistanta en brulkonsumado de diversaj
      substancoj, kun produktado de lumo kaj varmo:
      <ekz>
        fari <klr>(ekbruligi)</klr> <tld/>on
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        bloveksciti la <tld/>on
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;irka&ubreve;a&jcirc;o <klr>[&#x2026;]</klr> dezertigita per <tld/>o kaj glavo
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        bivakaj <tld/>oj flamis
        <fnt>
          <bib>Fab4</bib>
          <lok>Lasta son&gcirc;o de maljuna kverko</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili man&gcirc;u la viandon <klr>[&#x2026;]</klr> rostitan sur <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 12:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        fulgokovrita poto &scirc;a&ubreve;mis kaj vaporis super la flamanta ken<tld/>o
        <fnt>
          <bib>HsT</bib>
          <lok>Feli&ccirc;a homo</lok>
        </fnt>;
      </ekz>
      <ekz>
        kamen<tld/>o &ccirc;iam kreas tiel pla&ccirc;an etoson
        <fnt>
          <bib>CKv</bib>
          <lok>27</lok>
        </fnt>;
      </ekz>
      <ekz>
        el flamo sin eltiris, en <tld/>on eniris
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        tro akra <tld/>o estas sen da&ubreve;ro
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flam.0o">flamo</ref>,
      <ref cel="ard">ardo</ref>
    </refgrp>
    <trd lng="bo">&t_ma;&t_drengbu;&t_tsheg;</trd>
    <trd lng="br">tan</trd>
    <trd lng="ca">foc</trd>
    <trd lng="en">fire</trd>
    <trd lng="ja">&#x706B; [<ind>&#x3072;</ind>]</trd>
    <trd lng="pl">ogie&nacute;</trd>
    <trdgrp lng="pt">
      <trd>fogo</trd>,
      <trd>lume</trd>
    </trdgrp>
    <trd lng="tok">seli</trd>
    <trd lng="vo">fil</trd>
  </snc>
  <snc mrk="fajr.0o.fervoro">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref cel="entuzi.0o">Entuziasmo</ref>,
      granda <ref cel="fervor.0o">fervoro</ref>,
      <ref cel="ard.0o">ardeco</ref>:
      <ekz>
        en la <tld/>o de mia kolero
        <fnt><bib>MT</bib>, <lok>&Jeh; 38:19</lok></fnt>;
      </ekz>
      <ekz>
        diskuti kun <tld/>o;
      </ekz>
      <ekz>
        nun, &ndash; ekkriis li kun <tld/>o, &ndash; nun mi havas celon en la vivo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i reblovas en li la <tld/>on de l&#x2019; ven&gcirc;emo
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        de tempo al tempo &ccirc;iam denove ekflamadis en li la malnova <tld/>o de juneco
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        varmaj brunaj okuloj plenaj je <tld/>o, kis-invitaj lipoj
        <fnt>
          <bib>ChM</bib>
          <lok>6</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi incitas vin pro la plezuro rigardi la <tld/>on de via animo
        <fnt>
          <bib>ChC</bib>
          <lok>24</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekzalt">ekzalteco</ref>,
      <ref cel="febr.0o.fervoro">febro<sncref/></ref>,
      <ref cel="flam.0o.fervoro">flamo<sncref/></ref>,
      <ref cel="pasi.0o">pasio</ref>,
      <ref cel="varm.0o">varmo</ref>,
      <ref cel="verv.0o">vervo</ref>,
      <ref cel="vigl">vigleco</ref>
    </refgrp>
    <trdgrp lng="br">
      <trd>entan</trd>,
      <trd>birvidigezh</trd>,
      <trd>gred</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>entusiasme</trd>,
      <trd>passi&oacute;</trd>,
      <trd>enardiment</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>ardor</trd>,
      <trd>enthusiasm</trd>,
      <trd>fervor</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>ogie&nacute;</trd>,
      <trd>zapa&lstroke;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>fogo</trd>,
      <trd>entusiasmo</trd>
    </trdgrp>
  </snc>
