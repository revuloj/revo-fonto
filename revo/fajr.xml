<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>fajr</rad>/o
</kap>
<drv mrk="fajr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc mrk="fajr.0o.FIZ">
    <dif>
      Fenomeno konsistanta en brulkonsumado de diversaj substancoj, kun
      produktado de lumo kaj varmo:
      <ekz>
        fari <klr>(ekbruligi)</klr> <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        bloveksciti la <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        dezertigi landon per <tld/>o kaj glavo
        <fnt>Z</fnt>;
      </ekz>
      <ekz>tro akra <tld/>o estas sen da&ubreve;ro<fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flam.0o">flamo</ref>,
      <ref cel="ard">ardo</ref>.
    </refgrp>
  </snc>
  <snc mrk="fajr.0o.fervoro">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Entuziasmo, granda fervoro, ardeco:
      <ekz>
        en la <tld/>o de mia kolero
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Je&hcirc;ezkel 38, 19</lok>)</fnt>;
      </ekz>
      <ekz>
        diskuti kun <tld/>o;
      </ekz>
      <ekz>
        reblovi en iu la <tld/>on de la ven&gcirc;emo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ard.0eco">ardo</ref>,
      <ref cel="entuzi.0o">entuziasmo</ref>,
      <ref cel="ekzalt">ekzalteco</ref>,
      <ref cel="fervor.0o">fervoro</ref>,
      <ref cel="flam.0o.fervoro">flamo</ref>,
      <ref cel="febr.0o.fervoro">febro</ref>,
      <ref cel="pasi.0o">pasio</ref>,
      <ref cel="verv.0o">vervo</ref>,
      <ref cel="varm.0o.fervoro">varmo</ref>,
      <ref cel="vigl">vigleco</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="incit.0i">inciti</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">ohe&ncaron;</trd>
  <trd lng="de">Feuer</trd>
  <trd lng="en">fire</trd>
  <trd lng="fr">feu</trd>
  <trd lng="nl">vuur</trd>
  <trd lng="ru">&c_o;&c_g;&c_o;&c_n;&c_mol;</trd>
  <trd lng="sv">eld</trd>
</drv>

<drv mrk="fajr.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      De <tld/>o, simila al <tld/>o, havanta econ de <tld/>o:
      <ekz>
        la interna&jcirc;o de la tero konsistas el <tld/>a fandita
        maso;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a ardo, brilo<fnt>B</fnt>, deziro<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>aj okuloj, rigardoj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>de <ind>feu</ind></trd>,
    <trd>fougueux</trd>,
    <trd>ign&eacute;</trd>,
    <trd>enflamm&eacute;<klr tip="ind"> (passionn&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="nl">vurig</trd>
  <trd lng="ru">&c_o;&c_g;&c_n;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="fajr.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      En <tld/>a maniero:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>e ami, paroli.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">feurig</trd>
  <trdgrp lng="fr">
    <trd>avec <ind>feu</ind></trd>,
    <trd>avec <ind>fougue</ind></trd>,
    <trd>fougueusement</trd>,
    <trd>passionn&eacute;ment</trd>
  </trdgrp>
  <trd lng="nl">vurig</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_m;&c_je;&c_n;&c_n;&c_o;</trd>
</drv>

<drv mrk="fajr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Bruli kun <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">flamber</trd>
  <trdgrp lng="nl">
    <trd>aansteken</trd>,
    <trd>flamberen</trd>
  </trdgrp>
</drv>

<drv mrk="fajr.0ero">
  <kap><tld/>ero</kap>
  <uzo tip="fak">FIZ</uzo>
  <dif>
    Arda parteto
  </dif>
  <snc>
    <dif>
      el&jcirc;etita de brulanta korpo.
    </dif>
  </snc>
  <snc>
    <dif>
      produktata per frotado de du malmolaj korpoj.
    </dif>
  </snc>
  <snc>
    <dif>
      eligata de <ref cel="elektr.0umi">elektrumita</ref> korpo.
    </dif>
    <refgrp tip="sin">
      <ref cel="elektr.0afajrero">elektra <tld/>ero</ref>,
    </refgrp>
    <adm>
      Vd adm sub elektr pri elektrumi. Cetere "elektra fajrero"
      pli bone kusxus cxi tie ol sub "elektr". <aut>MB</aut>
    </adm>
  </snc>
  <refgrp tip="sin">
    <ref cel="spark.0o">sparko</ref>
  </refgrp>
  <trd lng="de">Funke</trd>
  <trd lng="fr">&eacute;tincelle</trd>
  <trd lng="nl">vonk</trd>
  <trd lng="ru">&c_i;&c_s;&c_k;&c_r;&c_a;</trd>
</drv>

<drv mrk="fajr.0ilo">
  <kap><tld/>ilo<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Malgranda ilo por produkti <tld/>erojn per frotado de silik&scirc;tono kaj
      &scirc;talpeco, a&ubreve; per alia procedo.
    </dif>
  </snc>
  <trd lng="de">Feuerzeug</trd>
  <trd lng="fr">briquet</trd>
  <trd lng="nl">aansteker</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_je;&c_s;&c_a;&c_l;&c_o;</trd>,
    <trd>&c_z;&c_a;&c_zh;&c_i;&c_g;&c_a;&c_l;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="fajr.0ostango">
  <kap><tld/>ostango</kap>
  <snc>
    <dif>
      Pikilo por eksciti <tld/>on.
    </dif>
  </snc>
  <trd lng="de">Sch&uuml;rhaken</trd>
  <trd lng="fr">tisonnier</trd>
  <trd lng="nl">pook</trd>
  <trd lng="ru">&c_k;&c_o;&c_ch;&c_je;&c_r;&c_g;&c_a;</trd>
</drv>

<drv mrk="fajr.0oprenilo">
  <kap><tld/>oprenilo<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Dubraka ilo por preni brulantan materion.
    </dif>
  </snc>
  <trd lng="de">Feuerzange</trd>
  <trd lng="fr">pince &agrave; feu</trd>
  <trd lng="ru">&c_shch;&c_i;&c_p;&c_c;&c_y; &c_d;&c_l;&c_ja; &c_u;&c_g;&c_l;&c_je;&c_j;</trd>
</drv>

<drv mrk="fajr.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Loko, en kiu <tld/>o brulas.
    </dif>
  </snc>
  <trd lng="de">Feuerstelle</trd>
  <trdgrp lng="fr">
    <trd>chemin&eacute;e<klr tip="ind"> (foyer)</klr></trd>,
    <trd>foyer<klr tip="ind"> (chemin&eacute;e)</klr></trd>
  </trdgrp>
  <trd lng="sv">eldstad</trd>
</drv>

<drv mrk="fajr.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <dif>
      Parto de forno a&ubreve; kameno destinita por enteni la <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>foyer<klr tip="ind"> (&acirc;tre)</klr></trd>,
    <trd>&acirc;tre</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_ch;&c_a;&c_g;</trd>
</drv>

<drv mrk="fajr.art0ajxo">
  <kap>(art)<tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Arta <tld/>o, konsistanta el raketoj kaj
      petardoj, kiujn oni ekbruligas en festoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="ilumin">ilumina&jcirc;o</ref>,
      <ref cel="pirote">piroteknika&jcirc;o</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Feuerwerk</trd>
  <trd lng="fr">feu d'artifice</trd>
  <trd lng="nl">vuurwerk</trd>
  <trd lng="ru">&c_f;&c_je;&c_j;&c_je;&c_r;&c_v;&c_je;&c_r;&c_k;</trd>
</drv>

<drv mrk="fajr.0ajxisto">
  <kap><tld/>a&jcirc;isto</kap>
  <snc>
    <dif>
      Tiu, kiu faras art<tld/>a&jcirc;ojn.
    </dif>
  </snc>
  <trd lng="de">Feuerwerker</trd>
  <trd lng="fr">artificier<klr tip="ind"> (pyrotechnicien)</klr></trd>
  <trd lng="nl">vuurwerkmaker</trd>
</drv>

<drv mrk="fajr.0estingisto">
  <kap><tld/>estingisto</kap>
  <snc>
    <dif>
      Ano de korporacio, speciale komisiita por estingi brulegojn;
      brulestingisto, fajrobrigadano.
    </dif>
  </snc>
  <adm>
    Vd adm sub esting. <aut>MB</aut>
  </adm>
  <trd lng="de">Feuerwehrmann</trd>
  <trd lng="fr">pompier<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="nl">brandweerman</trd>
  <trd lng="ru">&c_p;&c_o;&c_zh;&c_a;&c_r;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
