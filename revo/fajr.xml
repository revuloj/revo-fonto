  <trd>ohe&ncaron;</trd>,
  <trd>t&aacute;bor&aacute;k</trd>
  </trdgrp>
  <trdgrp lng="es">
      <trd>hoguera</trd>,
      <trd>fogata</trd>
    </trdgrp>
  <trd lng="id"><ind>api</ind> unggun</trd>
  <trdgrp lng="sk">
  <trd>hranica</trd>,
  <trd>ohe&ncaron;</trd>,
  <trd>t&aacute;bor&aacute;k</trd>
  </trdgrp>
</drv>

