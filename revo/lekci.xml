  <ofc>3</ofc>
  <rad>lekci</rad>/o
  <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="lekci.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PED</uzo>
  <snc mrk="lekci.0o.leciono">
    <dif>
      <ref tip="super" cel="lecion.0o.kuna">Leciono</ref> en formo de
      scienc-instrua parolado en universitato a&ubreve; alia samranga
      instituto:
      <ekz>
        la studentoj malfruis al la komenco de la <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la studentoj venas de la <tld/>oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Malbonkonduta knabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        universitata <ind><tld/>isto</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Princo Henriko Oguinye</aut>
          <vrk><url ref="&Monato;/2010/010080.php">Bon&scirc;anca prezidanto</url></vrk>
          <lok>2010</lok>
        </fnt>;
        <trd lng="de">Dozent</trd>
      </ekz>
      <ekz>
        krom doni teoriajn <tld/>ojn, mi anka&ubreve; trejnas lingvoinstruistojn pri metodologio
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Monato;/2003/008010.html">Katalin Kov&aacute;ts</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> profesoro eldiris sian ina&ubreve;guran <tld/>on <klr>[&#8230;]</klr> pri
        Korano kaj Hadiso
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="&Monato;/2011/010194.php">Islama universitato rekonita</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="preleg.0o">prelego</ref>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Anka&#365; eblas diri (universitata) <ke>prelego</ke>.
  </rim>

  <trd lng="be">&c_l;&c_je;&c_k;&c_c;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_l;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>
  <trd lng="ca">lli&ccedil;&oacute; <klr tip="ind">(universit&agrave;ria)</klr></trd>
  <trd lng="cs">p&rcaron;edn&aacute;&scaron;ka na vys. &scaron;kole</trd>
  <trdgrp lng="de">
    <trd>Unterrichtseinheit</trd>,
    <trd>Vorlesung</trd>
  </trdgrp>
  <trd lng="en">lecture</trd>
  <trdgrp lng="fr">
    <trd>cours magistral</trd>
  </trdgrp>
  <trd lng="hu">el&odblac;ad&aacute;s <klr tip="ind">(egyetemi)</klr></trd>
  <trd lng="ils" kod="M552x567S2ff00482x483S15a1a516x555S3770e491x559S37601525x515S15a10532x503S26a00525x489"/>
  <trd lng="ja">&#35611;&#32681;  <pr>&#12371;&#12358;&#12366;</pr></trd>
  <trd lng="nl">college</trd>
  <trd lng="pl">wyk&lstroke;ad</trd>
  <trdgrp lng="pt">
    <trd>prele&ccedil;&atilde;o</trd>,
    <trd>aula <klr tip="amb">(universit&aacute;ria)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sgn" kod="M535x518S1dc20511x468S1dc28465x468S31500482x483S20500507x475S20500483x474">l@01*</trd>
  <trdgrp lng="sk">
    <trd>predn&aacute;&scaron;ka</trd>,
    <trd>v&yacute;klad u&ccaron;ite&lcaron;a na V&Scaron;</trd>
  </trdgrp>
  <trd lng="sv">f&ouml;rel&auml;sning</trd>
  <trd lng="uk">&#1083;&c_je;&#1082;&c_c;&c_ib;&#1103;</trd>
  <trdgrp lng="zh">
    <trd>&#35611;&#36781; <pr>ji&#462;ngc&#237;</pr></trd>,
    <trd>&#35762;&#36766; <pr>ji&#462;ngc&#237;</pr></trd>,
    <trd>&#35611;&#35506; <pr>ji&#462;ngk&#232;</pr></trd>,
    <trd>&#35762;&#35838; <pr>ji&#462;ngk&#232;</pr></trd>,
    <trd>&#35611;&#24231; <pr>ji&#462;ngzu&#242;</pr></trd>,
    <trd>&#35762;&#24231; <pr>ji&#462;ngzu&#242;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lekci.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on:
      <ekz>
        la medreseo estis unu el la plej bonaj ekleziaj universitatoj de islama Oriento
        en la 15a jarcento, <nom>Ulugbek</nom> mem <tld/>is en &gcirc;i kaj gvidis disputojn kun
        studentoj kaj sciencistoj
        <fnt>
          <bib>Monato</bib>
          <aut>Anatoli Ionesov</aut>
          <vrk><url ref="&Monato;/2006/008920.php">Perlo &ccirc;e la Silka Vojo</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        sian gepatran lingvon <klr>[&#8230;]</klr> studentoj e&ccirc; ne plu bezonas, &ccirc;ar en
        universitatoj oni <tld/>as per la angla
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2006/008963.php">Germana batalo</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Anka&#365; eblas diri <ke>prelegi</ke> (en universitato).
  </rim>

  <trd lng="be">&c_ch;&c_y;&c_t;&c_a;&c_c;&c_mol; <ind>&c_l;&c_je;&c_k;&c_c;&c_y;&c_ib;</ind></trd>
  <trd lng="bg">&c_p;&c_o;&c_u;&c_ch;&c_a;&c_v;&c_a;&c_m;</trd>
  <trd lng="ca">impartir <klr tip="ind">(una lli&ccedil;&oacute;
  universit&agrave;ria)</klr></trd>
  <trd lng="cs">p&rcaron;edn&aacute;&scaron;et na vys. &scaron;kole</trd>
  <trdgrp lng="de">
    <trd>dozieren</trd>,
    <trd><klr>eine </klr>Vorlesung <ind>halten</ind></trd>
  </trdgrp>
  <trd lng="en">lecture</trd>
  <trdgrp lng="fr">
    <trd>faire <ind>cours</ind></trd>
  </trdgrp>
  <trd lng="hu">el&odblac;ad <klr tip="ind">(egyetemen)</klr></trd>
  <trd lng="ja">&#35611;&#32681;&#12434;&#12377;&#12427;  <pr>&#12371;&#12358;&#12366;&#12434;&#12377;&#12427;</pr></trd>
  <trd lng="nl">college geven</trd>
  <trdgrp lng="pl">
    <trd>wyk&lstroke;ada&cacute;</trd>,
    <trd>dawa&cacute; <ind>wyk&lstroke;ad</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_i;&c_t;&c_a;&c_t;&c_mol; <ind>&c_l;&c_je;&c_k;&c_c;&c_i;&c_i;</ind></trd>
  <trd lng="sk">predn&aacute;&scaron;a&tcaron; <klr>(na vysokej &scaron;kole)</klr></trd>
  <trd lng="sv">f&ouml;rel&auml;sa</trd>
  <trd lng="uk">&c_ch;&c_i;&c_t;&c_a;&c_t;&c_i; &#1083;&c_je;&#1082;&c_c;&c_ib;&#1102;</trd>
  <trdgrp lng="zh">
    <trd>&#35611;&#28436; <pr>ji&#462;ngy&#462;n</pr></trd>,
    <trd>&#35762;&#28436; <pr>ji&#462;ngy&#462;n</pr></trd>,
    <trd>&#20027;&#35611; <pr>zh&#468;ji&#462;ng</pr></trd>,
    <trd>&#20027;&#35762; <pr>zh&#468;ji&#462;ng</pr></trd>,
    <trd>&#28436;&#35611; <pr>y&#462;nji&#462;ng</pr></trd>,
    <trd>&#28436;&#35762; <pr>y&#462;nji&#462;ng</pr></trd>,
    <trd>&#35611;&#35506; <pr>ji&#462;ngk&#232;</pr></trd>,
    <trd>&#35762;&#35838; <pr>ji&#462;ngk&#232;</pr></trd>
  </trdgrp>
</drv>
