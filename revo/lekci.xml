<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>3</ofc>
      <rad>lekci</rad>/o
    </kap>
    <drv mrk="lekci.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          <ref tip="super" cel="lecion.0o.kuna">Leciono</ref> en formo de
          scienc-instrua parolado en universitato a&ubreve; alia samraranga
          instituto:
          <ekz>
            <tld/>oj pri la scienco de la lingvo<fnt>Z</fnt>;
          </ekz><ekz>
            kiam la studentoj venas de la <tld/>oj<fnt>Z</fnt>.
          </ekz> 
        </dif>
        <ref tip="sin" cel="preleg.0o">prelego</ref>
      </snc>
      <trd lng="ru">&c_l;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>
      <trd lng="sv">f&ouml;rel&auml;sning</trd>
    </drv>
    <drv mrk="lekci.0i">
      <kap><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
        <dif>
          Fari <tld/>o(j)n.  
        </dif>
      </snc>
      <trd lng="ru">&c_ch;&c_i;&c_t;&c_a;&c_t;&c_mol; <ind>&c_l;&c_je;&c_k;&c_c;&c_i;&c_i;</ind></trd>
      <trd lng="sv">f&ouml;rel&auml;sa</trd>
    </drv>
  </art>
