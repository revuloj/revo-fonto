<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc> <rad>ter</rad>/o
</kap>
<drv mrk="ter.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="ter.0o.planedo">
    <dif>
      La <ref tip="super" cel="planed.0o">planedo</ref>,
      sur kiu la homoj lo&gcirc;as.
    </dif>
    <ref tip="sin" cel="ter.0globo"><tld/>globo</ref>
    <ref tip="vid" cel="mond.0o.tero">mondo</ref>
    <trd lng="be">&c_Z;&c_ja;&c_m;&c_l;&c_ja;</trd>
    <trd lng="he">&kaf;&dalet;&vav;&resh; &he;&alef;&resh;&ftsadi;</trd>
    <trd lng="hu">F&ouml;ld</trd>
    <trdgrp lng="nl">
      <trd>aarde</trd>,
      <trd>aardbol</trd>
    </trdgrp>
    <trd lng="ru">&c_Z;&c_je;&c_m;&c_l;&c_ja;</trd>
    <trdgrp lng="tr">
      <trd>d&uuml;nya</trd>,
      <trd>Yer</trd>
    </trdgrp>
    <trd lng="zh">&#22320;&#29699;<klr> [d&igrave;qi&uacute;]</klr></trd>
  </snc>
  <snc mrk="ter.0o.surfaco">
    <dif>
      Ekstera firma tavolo de &ccirc;i tiu planedo:
      <ekz>
        super la <tld/>o sin trovas aero
        <fnt><bib>F</bib>, <lok>&FE; 26</lok></fnt>;
      </ekz>
      <ekz>
        ku&scirc;i sur la <tld/>o;
      </ekz>
      <ekz>
        la <tld/>o kaj la maro;
      </ekz>
      <ekz>
        fosi la <tld/>on;
      </ekz>
      <ekz>
        la rajdanto desaltis <tld/>en;
      </ekz>
      <ekz>
        fali <tld/>en;
      </ekz>
      <ekz>
        ili &jcirc;eti&gcirc;is viza&gcirc;al<tld/>e<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="he">&qof;&resh;&qof;&ayin;</trd>
    <trd lng="hu">f&ouml;ld</trd>
    <trdgrp lng="nl">
      <trd>grond</trd>,
      <trd>land</trd>
    </trdgrp>
    <trdgrp lng="tr">
      <trd>yery&uuml;z&uuml;</trd>,
      <trd>d&uuml;nya</trd>
    </trdgrp>
    <trd lng="vo">glun</trd>
  </snc>
  <snc mrk="ter.0o.substanco">
    <dif>
      Substanco el kiu konsistas tiu tavolo:
      <ekz>
        poto el <tld/>o;
      </ekz>
      <ekz>
        argila, sabla, seka, kulturebla <tld/>o
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="argil.0o">argilo</ref>,
      <ref cel="grund.0o">grundo</ref>
    </refgrp>
    <trd lng="he">&alef;&dalet;&mem;&he;</trd>
    <trdgrp lng="hu">
      <trd>f&ouml;ld</trd>,
      <trd>agyag</trd>
    </trdgrp>
    <trd lng="tr">toprak</trd>
    <trdgrp lng="nl">
      <trd>aarde</trd>,
      <trd>grond</trd>
    </trdgrp>
    <trd lng="vo">glun</trd>
  </snc>
  <trd lng="be">&c_z;&c_ja;&c_m;&c_l;&c_ja;</trd>
  <trd lng="cs">zem</trd>
  <trd lng="de">Erde</trd>
  <trd lng="en">earth</trd>
  <trd lng="es">tierra</trd>
  <trd lng="fr">terre</trd>
  <trd lng="he">&alef;&resh;&ftsadi;</trd>
  <trd lng="ru">&c_z;&c_je;&c_m;&c_l;&c_ja;</trd>
  <trd lng="zh">&#22303;<klr> [t&#468;]</klr></trd>
</drv>

<drv mrk="ter.0a">
  <kap><tld/>a</kap>
  <snc mrk="ter.0a.planeda">
    <dif>
      Apartenanta al la planedo <tld/>o:
      <ekz>
        la <tld/>a supra&jcirc;o;
      </ekz>
      <ekz>
        la <tld/>a diametro.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_ja;&c_m;&c_n;&c_y;</trd>
    <trd lng="es">terrestre</trd>
    <trd lng="he">&alef;&resh;&tsadi;&yod;</trd>
    <trdgrp lng="hu">
      <trd>f&ouml;ldi</trd>,
      <trd>f&ouml;ld-</trd>
    </trdgrp>
    <trd lng="nl">aard-</trd>
    <trd lng="ru">&c_z;&c_je;&c_m;&c_n;&c_o;&c_j;</trd>
  </snc>
  <snc mrk="ter.0a.substanca">
    <dif>
      El la substanco de la tero:
      <ekz>
        <tld/>a poto.
      </ekz>
    </dif>
    <ref tip="sub" cel="argil.0a">argila</ref>
    <trd lng="be">&c_z;&c_je;&c_m;&c_l;&c_ja;&c_n;&c_y;</trd>
    <trd lng="de">irden</trd>
    <trdgrp lng="hu">
      <trd>f&ouml;ldes</trd>,
      <trd>f&ouml;ld-</trd>,
      <trd>agyag-</trd>
    </trdgrp>
    <trd lng="nl">aarden</trd>
    <trd lng="ru">&c_z;&c_je;&c_m;&c_l;&c_ja;&c_n;&c_o;&c_j;</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>en <ind>terre</ind></trd>,
    <trd>terrestre</trd>
  </trdgrp>
</drv>

<drv mrk="ter.al0igxi">
  <kap>al<tld/>i&gcirc;i</kap>
  <snc mrk="ter.al0igxi.KOMUNE">
    <dif>
      Alveni al la <tld/>a surfaco.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_z;&c_ja;&c_m;&c_l;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trd lng="de">landen</trd>
  <trd lng="es">aterrizar</trd>
  <trd lng="fr">atterrir</trd>
  <trd lng="he">&lamed;&nun;&het;&vav;&tav;
  \<ind>&nun;&het;&tav;</ind>\</trd>
  <trdgrp lng="hu">
    <trd>f&ouml;ldet &eacute;r</trd>,
    <trd>landol</trd>
  </trdgrp>
  <trd lng="nl">landen</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_z;&c_je;&c_m;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="ter.ekster0ano">
  <kap>ekster<tld/>ano</kap>
  <snc mrk="ter.ekster0ano.BELE">
    <dif>
      Supozata vivulo pli-malpli homsimila, kiu ne venus de la planedo
      <tld/>o:
      <ekz>
        e&ccirc; se vi estus ekster<tld/>ano vi rajtus kritiki la malbona&jcirc;ojn de
        la <tld/>o
        <fnt>
        <aut>E. Fabian</aut>
        <vrk>R: Kie estas la stranguloj?</vrk>
        <lok>soc.culture.esperanto, 2002-05-25</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">extra-terrestre<klr tip="amb"> (subst.)</klr></trd>
  <trd lng="ru">&c_i;&c_n;&c_o;&c_p;&c_l;&c_a;&c_n;&c_je;&c_t;&c_ja;&c_n;&c_i;&c_n;</trd>
</drv>

<drv mrk="ter.en0igi">
  <kap><ofc>*</ofc>en<tld/>igi</kap>
  <snc mrk="ter.en0igi.en_tero">
    <dif>
      Meti en la <tld/>on:
      <ekz>
        en<tld/>igi trezoron, markoton;
      </ekz>
      <ekz>
        kiu eltrinkos la pokalojn <klr>...</klr> povos uzi la glavon,
        kiu estas en<tld/>igita anta&ubreve; la sojlo
        <fnt><bib>ElektFab</bib>, <lok>du fratoj</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_k;&c_a;&c_p;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_r;&c_y;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">eingraben</trd>
    <trdgrp lng="he">
      <trd>&lamed;&tet;&mem;&vav;&fnun;
      \<ind>&tet;&mem;&fnun;</ind>\</trd>,
      <trd>&lamed;&he;&tet;&mem;&yod;&fnun;
      \<ind>&tet;&mem;&fnun;</ind>\</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>el&aacute;s</trd>,
      <trd>f&ouml;ldbe &aacute;s</trd>
    </trdgrp>
    <trd lng="nl">ingraven</trd>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_k;&c_o;&c_p;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_r;&c_y;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="ter.en0igi.entombigi">
    <dif>
      <ref tip="super" cel="sepult.0i">Sepulti</ref> metante la korpon
      de mortinto en sub<tld/>an tombon:
      <ekz>
        permesu al mi unue iri kaj en<tld/>igi mian patron
        <fnt><bib>NT</bib>, <lok>&Luk; 9:59</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_h;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_h;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">begraben</trd>
    <trd lng="he">&lamed;&qof;&bet;&vav;&resh;
    \<ind>&qof;&bet;&resh;</ind>\</trd>
    <trdgrp lng="hu">
      <trd>eltemet</trd>,
      <trd>elhantol</trd>
    </trdgrp>
    <trd lng="nl">begraven</trd>
    <trdgrp lng="ru">
      <trd>&c_h;&c_o;&c_r;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_o;&c_h;&c_o;&c_r;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_h;&c_o;&c_r;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="ter.en0igi.forgesigi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Forgesigi, vanigi, neniigi:
      <ekz>
        tiu projekto estas jam tute
        senkreditigita kaj en<tld/>igita<fnt>Z</fnt>;
      </ekz>
      <ekz>
        eble nia afero &scirc;ajnos al la mondo e&ccirc; mortinta
        kaj jam en <tld/>igita<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_h;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="de">begraben</trd>
    <trd lng="hu">eltemet</trd>
    <trd lng="nl">begraven</trd>
    <trd
    lng="ru">&c_p;&c_o;&c_h;&c_o;&c_r;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="es">enterrar</trd>
  <trd lng="fr">enterrer</trd>
</drv>

<drv mrk="ter.el0igi">
  <kap>el<tld/>igi</kap>
  <snc mrk="ter.el0igi.KOMUNE">
    <dif>
      Eltiri el la <tld/>o:
      <ekz>
        el<tld/>igado de ercoj
        <fnt>
        <aut>Josef Mendl</aut>,
        <vrk>Konsekvencoj de ekonomiaj transformoj</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2002a, numero 12a, p. 10a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_k;&c_a;&c_p;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_k;&c_a;&c_p;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_a;&c_p;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">ausgraben</trd>
  <trd lng="es">desenterrar</trd>
  <trd lng="fr">d&eacute;terrer</trd>
  <trdgrp lng="hu">
    <trd>ki&aacute;s</trd>,
    <trd>kihantol</trd>
  </trdgrp>
  <trd lng="nl">opgraven</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_k;&c_o;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_p;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="ter.sub0ajxo">
  <kap>sub<tld/>a&jcirc;o</kap>
  <snc mrk="ter.sub0ajxo.spaco">
    <dif>
      Natura a&ubreve; arta spaco sub la <tld/>o:
      <ekz>
        ili iris malsupren en la sub<tld/>a&jcirc;ojn de la templo de
        Ptah kaj balda&ubreve; atingis vastan kelon
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 14a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="kel.0o">kelo</ref>,
      <ref cel="tunel.0o">tunelo</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_d;&c_v;&c_a;&c_l;</trd>,
      <trd>&c_p;&c_a;&c_d;&c_z;&c_ja;&c_m;&c_je;&c_l;&c_mol;&c_l;&c_je;</trd>,
      <trd>&c_s;&c_k;&c_l;&c_je;&c_p;</trd>,
      <trd>&c_s;&c_u;&c_t;&c_a;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trd lng="fr">souterrain<klr> (subst.)</klr></trd>
    <trd lng="he">&mem;&nun;&he;&resh;&he;</trd>
    <trdgrp lng="hu">
      <trd>f&ouml;ld alatti j&aacute;rat</trd>,
      <trd>f&ouml;ld alatti &eacute;p&iacute;tm&eacute;ny</trd>
    </trdgrp>
    <trd lng="nl">onderaardse <ind>gang</ind></trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_d;&c_z;&c_je;&c_m;&c_je;&c_l;&c_mol;&c_je;</trd>,
      <trd>&c_p;&c_o;&c_d;&c_v;&c_a;&c_l;</trd>
    </trdgrp>
    <adm>
      Mi &gcirc;eneraligis la difinon, kiu ampleksis nur "galerion
      fositan". La tradukoj eble ne plu tute &gcirc;ustas.
      <aut>&jcirc;v</aut>
    </adm>
  </snc>
</drv>

<drv mrk="ter.sur0a">
  <kap>sur<tld/>a</kap>
  <snc mrk="ter.sur0a.KOMUNE">
    <dif>
      Okazanta a&ubreve; vivanta sur la <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_z;&c_je;&c_m;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_ja;&c_m;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="fr">terrestre</trd>
  <trd lng="hu">f&ouml;ldi</trd>
  <trdgrp lng="nl">
    <trd>bovenaards</trd>,
    <trd>bovengronds</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_z;&c_je;&c_m;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_z;&c_je;&c_m;&c_n;&c_o;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="ter.0globo">
  <kap><tld/>globo</kap>
  <snc mrk="ter.0globo.AST">
    <ref tip="dif" cel="ter.0o.planedo"><tld/>o <sncref/></ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_Z;&c_ja;&c_m;&c_l;&c_ja;</trd>,
    <trd>&c_z;&c_ja;&c_m;&c_n;&c_y; &c_sh;&c_a;&c_r;</trd>,
    <trd>&c_z;&c_ja;&c_m;&c_n;&c_a;&c_ja; &c_k;&c_u;&c_l;&c_ja;</trd>
  </trdgrp>
  <trd lng="de">Erdkugel</trd>
  <trd lng="es">globo terr&aacute;queo</trd>
  <trd lng="fr">globe terrestre</trd>
  <trd lng="he">&kaf;&dalet;&vav;&resh; &he;&alef;&resh;&ftsadi;</trd>
  <trd lng="hu">f&ouml;ldgoly&oacute;</trd>
  <trdgrp lng="nl">
    <trd>aardbol</trd>,
    <trd>aarde</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_Z;&c_je;&c_m;&c_l;&c_ja;</trd>,
    <trd>&c_z;&c_je;&c_m;&c_n;&c_o;&c_j; &c_sh;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="zh">&#22320;&#29699;<klr> [d&igrave;qi&uacute;]</klr></trd>
</drv>

<drv mrk="ter.0kapo">
  <kap><tld/>kapo</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="ter.0kapo.GEOG">
    <dif>
      Terpinto anta&ubreve;i&gcirc;anta en la maron.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="kab.0o">kabo</ref>,
    <ref cel="kap.0o.GEOG">kapo <sncref/></ref>,
    <ref cel="promon.0o">promontoro</ref>
  </refgrp>
  <ref tip="vid" cel="ter.0lango"><tld/>lango</ref>
  <trd lng="be">&c_m;&c_y;&c_s;</trd>
  <trdgrp lng="de">
    <trd>Kap</trd>,
    <trd>Landspitze</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cape</trd>,
    <trd>promontory</trd>
  </trdgrp>
  <trd lng="es">cabo</trd>
  <trdgrp lng="fr">
    <trd>cap</trd>,
    <trd>promontoire</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fok<klr tip="amb"> (tengerbe ny&uacute;l&oacute;)</klr></trd>,
    <trd>hegyfok</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kaap</trd>,
    <trd>voorgebergte</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_y;&c_s;</trd>
  <trd lng="sv">udde</trd>
  <trd lng="tr">burun</trd>
</drv>

<drv mrk="ter.0konekto">
  <kap><tld/>konekto</kap>
  <snc mrk="ter.0konekto.ELE">
    <uzo tip="fak">ELE</uzo>
    <uzo tip="fak">ELET</uzo>
    <uzo tip="fak">RAD</uzo>
    <dif>
      Tiu konekto per drato inter elektra aparato kaj la <tld/>o,
      kiun oni faras por utiligi la <tld/>surfacon kiel
      unu platon de granda kondensatoro, kies alia plato povas
      esti la <ref tip="vid" cel="anten.0o.ELE">anteno</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_z;&c_ja;&c_m;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="fr">terre<klr> (&eacute;lectricit&eacute;)</klr></trd>
  <trd lng="he">&he;&alef;&resh;&qof;&he;</trd>
  <trd lng="hu">f&ouml;ldel&eacute;s</trd>
  <trd lng="nl">aarding</trd>
  <trd lng="ru">&c_z;&c_a;&c_z;&c_je;&c_m;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="ter.0kontakto">
  <kap><tld/>kontakto</kap>
  <snc mrk="ter.0kontakto.ELE">
    <uzo tip="fak">ELE</uzo>
    <uzo tip="fak">ELET</uzo>
    <dif>
      Okaza kontakto de elektra kondukilo kun la <tld/>o, kien
      elfluas kaj perdi&gcirc;as parto de la kurento de la cirkvito.
    </dif>
    <ref tip="vid" cel="lik.0i">liki</ref>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_m;&c_y;&c_k;&c_a;&c_n;&c_mol;&c_n;&c_je; &c_n;&c_a; &c_z;&c_ja;&c_m;&c_l;&c_ju;</trd>
  <trd lng="he">&he;&alef;&resh;&qof;&he;</trd>
  <trd lng="hu">f&ouml;ldr&ouml;vidz&aacute;rlat</trd>
  <trd lng="nl">aarding</trd>
  <trd lng="ru">&c_z;&c_a;&c_m;&c_y;&c_k;&c_a;&c_n;&c_i;&c_je; &c_n;&c_a; &c_z;&c_je;&c_m;&c_l;&c_ju;</trd>
</drv>

<drv mrk="ter.0krusto">
  <kap><tld/>krusto</kap>
  <snc mrk="ter.0krusto.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Solidi&gcirc;inta supera parto de la <tld/>globo,
      &ccirc;irka&ubreve;anta la centran varmegan kernon.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_ja;&c_m;&c_n;&c_a;&c_ja; &c_k;&c_a;&c_r;&c_a;</trd>
  <trd lng="de">Erdkruste</trd>
  <trd lng="es">corteza terrestre</trd>
  <trd lng="fr">&eacute;corce terrestre</trd>
  <trd lng="hu">f&ouml;ldk&eacute;reg</trd>
  <trd lng="nl">aardkorst</trd>
  <trd lng="ru">&c_z;&c_je;&c_m;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_r;&c_a;</trd>
</drv>

<drv mrk="ter.0kulturo">
  <kap><tld/>kulturo<fnt>Z</fnt></kap>
  <snc mrk="ter.0kulturo.AGR">
    <uzo tip="fak">AGR</uzo>
    <ref tip="dif" cel="agriku.0o">agrikulturo</ref>
    <ekz>
      unuj el ili sin okupis je <tld/>kulturo, aliaj je fi&scirc;kaptado a&ubreve;
      marveturado
      <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 17a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="es">agricultura</trd>
  <trd lng="fr">agriculture</trd>
  <trd lng="he">&het;&qof;&lamed;&alef;&vav;&tav;</trd>
  <trd lng="hu">f&ouml;ldm&odblac;vel&eacute;s</trd>
  <trd lng="nl">landbouw</trd>
  <trd lng="ru">&c_s;&c_je;&c_l;&c_mol;&c_s;&c_k;&c_o;&c_je; &c_h;&c_o;&c_z;&c_ja;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>

<drv mrk="ter.0laboristo">
  <kap><tld/>laboristo<fnt>Z</fnt></kap>
  <snc mrk="ter.0laboristo.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Salajrulo, kiu prilaboras la <tld/>on:
      <ekz>
        Kain fari&gcirc;is <tld/>laboristo
        <fnt><bib>MT</bib>, <lok>&Gen; 4:2</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="he">
    <trd>&ayin;&vav;&bet;&dalet; &alef;&dalet;&mem;&he;</trd>,
    <trd>&het;&qof;&lamed;&alef;&yod;</trd>
  </trdgrp>
  <trd lng="fr">ouvrier agricole</trd>
  <trdgrp lng="hu">
    <trd>b&eacute;res</trd>,
    <trd>mez&odblac;gazdas&aacute;gi <ind>munk&aacute;s</ind></trd>,
    <trd>mezei <ind>munk&aacute;s</ind></trd>
  </trdgrp>
  <trd lng="nl">landbouwer</trd>
  <trd lng="ru">&c_s;&c_je;&c_l;&c_mol;&c_s;&c_k;&c_o;&c_h;&c_o;&c_z;&c_ja;&c_j;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j; &c_r;&c_a;&c_b;&c_o;&c_ch;&c_i;&c_j;</trd>
</drv>

<drv mrk="ter.0lango">
  <kap><tld/>lango<fnt>Z</fnt></kap>
  <snc mrk="ter.0lango.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Longa, mallar&gcirc;a strio de <tld/>o, kiu etendi&gcirc;as en
      maron, lagon a&ubreve; riveron:
      <ekz>
        kelkaj rokplatoj ku&scirc;is kiel malgrandaj <tld/>langoj en la
        neprofunda akvo
        <fnt><bib>InfanTorent2</bib>, <lok>unua parto, &ccirc;apitro
        2a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="ter.0kapo"><tld/>kapo</ref>
  <trd lng="be">&c_k;&c_a;&c_s;&c_a; <klr tip="amb">(&c_g;&c_je;&c_a;&c_g;&c_r;.)</klr></trd>
  <trdgrp lng="fr">
    <trd>cap</trd>,
    <trd>langue de terre</trd>
  </trdgrp>
  <trd lng="he">&lamed;&shin;&vav;&fnun; &yod;&bet;&shin;&he;</trd>
  <trd lng="hu">f&ouml;ldnyelv</trd>
  <trd lng="nl">landtong</trd>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_a;<klr tip="amb"> (&c_g;&c_je;&c_o;&c_g;&c_r;.)</klr></trd>
</drv>

<drv mrk="ter.0mezuri">
  <kap><tld/>mezuri<fnt>Z</fnt></kap>
  <snc mrk="ter.0mezuri.GEOD">
    <uzo tip="fak">GEOD</uzo>
    <dif>
      Mezuri la surfacon de agro, arbaro, herbejo, dislimi ilin
      ktp:
      <ekz>
        la malnova <tld/>mezura stango
        <fnt><bib>Fab4</bib>, <lok>kuristoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">arpenter<klr> (mesurer les surf.)</klr></trd>
  <trd lng="hu">felm&eacute;r<klr tip="ind"> (f&ouml;ldet)</klr></trd>
  <trd lng="nl">landmeten</trd>
  <trd lng="ru">&c_m;&c_je;&c_zh;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="ter.0mezuristo">
  <kap><tld/>mezuristo</kap>
  <snc mrk="ter.0mezuristo.GEOD">
    <uzo tip="fak">GEOD</uzo>
    <dif>
      Tiu kies profesio estas <tld/>mezuri:
      <ekz>
        en samajn figurojn estas ja dividataj niaj kampoj, kaj
        la &scirc;tataj <tld/>mezuristoj lernas ilin en la lernejoj
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 2</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_m;&c_o;&c_r;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_z;&c_je;&c_m;&c_l;&c_ja;&c_m;&c_je;&c_r;</trd>
  </trdgrp>
  <trd lng="fr">arpenteur</trd>
  <trd lng="hu">f&ouml;ldm&eacute;r&odblac;</trd>
  <trd lng="nl">landmeter</trd>
  <trd lng="ru">&c_z;&c_je;&c_m;&c_l;&c_je;&c_m;&c_je;&c_r;</trd>
</drv>

<drv mrk="ter.0nukso">
  <kap><tld/>nukso</kap>
  <snc mrk="ter.0nukso.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Specio de <ref tip="super" cel="arakid.0o">arakido</ref>,
      kies frukto estas man&gcirc;ebla, kaj liveras oleon kaj specon de
      butero.
    </dif>
  </snc>
  <trd lng="es">cacahuate</trd>
  <trd lng="fr">cacahou&egrave;te</trd>
  <trd lng="he">&bet;&vav;&tet;&fnun;</trd>
  <trdgrp lng="hu">
    <trd>amerikai <ind>mogyor&oacute;</ind></trd>,
    <trd>f&ouml;ldimogyor&oacute;</trd>,
    <trd>b&uacute;rmogyor&oacute;</trd>
  </trdgrp>
  <trd lng="nl">aardnoot</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_a;&c_h;&c_i;&c_s;</trd>,
    <trd>&c_z;&c_je;&c_m;&c_l;&c_ja;&c_n;&c_o;&c_j; &c_o;&c_r;&c_je;&c_h;</trd>
  </trdgrp>
</drv>

<drv mrk="ter.0parto">
  <kap><tld/>parto<fnt>Z</fnt></kap>
  <snc mrk="ter.0parto.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <ref tip="dif" cel="mond.0oparto">mondoparto</ref>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_s;&c_t;&c_k;&c_a; &c_s;&c_mol;&c_v;&c_je;&c_t;&c_u;</trd>
  <trd lng="fr">r&eacute;gion du monde</trd>
  <trd lng="hu">f&ouml;ldr&eacute;sz</trd>
  <trd lng="nl">werelddeel</trd>
  <trd lng="ru">&c_ch;&c_a;&c_s;&c_t;&c_mol; &c_s;&c_v;&c_je;&c_t;&c_a;</trd>
</drv>

<drv mrk="ter.0peco">
  <kap><tld/>peco<fnt>Z</fnt></kap>
  <snc mrk="ter.0peco.GEOD">
    <dif>
      Pli-malpli granda peco de <tld/>o:
      <ekz>
        &ccirc;i tie <klr>...</klr> &ccirc;iu <tld/>peco estas historia
        <fnt><bib>Fab1</bib>, <lok>ligo de amikeco</lok></fnt>;
      </ekz>
      <ekz>
        sur la sama <tld/>peco oni konstruis belan domon
        <fnt><bib>Fab2</bib>, <lok>malnova domo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_je;&c_t;&c_a;&c_k;</trd>,
    <trd>&c_p;&c_l;&c_ja;&c_c;</trd>
  </trdgrp>
  <trd lng="fr">terrain</trd>
  <trd lng="he">&pe;&yod;&samekh;&tav; &qof;&resh;&qof;&ayin;</trd>
  <trd lng="hu">f&ouml;lddarab</trd>
  <trd lng="nl">landgoed</trd>
  <trd lng="ru">&c_u;&c_ch;&c_a;&c_s;&c_t;&c_o;&c_k; &c_z;&c_je;&c_m;&c_l;&c_i;</trd>
</drv>

<drv mrk="ter.0pomo">
  <kap><ofc>*</ofc><tld/>pomo</kap>
  <snc mrk="ter.0pomo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;ebla radikotubero de la <ref tip="malprt"
      cel="ter.0pomujo"><tld/>pomujo</ref>:
      <ekz>
        vi ricevos de mi sakon da <tld/>pomoj
        <fnt><bib>FK</bib>, <lok>3, anekdotoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_l;&c_mol;&c_b;&c_a;</trd>
  <trd lng="de">Kartoffel</trd>
  <trdgrp lng="es">
    <trd>papa</trd>,
    <trd>patata</trd>,
  </trdgrp>
  <trd lng="fr">pomme de terre</trd>
  <trd lng="he">&tav;&pe;&vav;&het; &alef;&dalet;&mem;&he;</trd>
  <trdgrp lng="hu">
    <trd>burgonya<klr tip="ind"> (term&eacute;s)</klr></trd>,
    <trd>krumpli<klr tip="ind"> (term&eacute;s)</klr></trd>
  </trdgrp>
  <trd lng="nl">aardappel<klr tip="ind"> (vrucht)</klr></trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_t;&c_o;&c_f;&c_je;&c_l;&c_mol;</trd>
  <trd lng="tr">patates</trd>
</drv>

<drv mrk="ter.0pomujo">
  <kap><ofc>*</ofc><tld/>pomujo</kap>
  <snc mrk="ter.0pomujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Specio de <ref tip="super" cel="solan.0o">solano</ref>
      (<trd lng="la">Solanum tuberosum</trd>), devena de Ameriko
      kun man&gcirc;eblaj radikotuberoj.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_l;&c_mol;&c_b;&c_ja;&c_t;&c_n;&c_ib;&c_k;</trd>
  <trd lng="de">Kartoffel<klr tip="ind"> (-pflanze)</klr></trd>
  <trd lng="fr">pomme de terre<klr tip="amb"> (plante)</klr></trd>
  <trdgrp lng="hu">
    <trd>burgonya<klr tip="ind"> (n&ouml;v&eacute;ny)</klr></trd>,
    <trd>krumpli<klr tip="ind"> (n&ouml;v&eacute;ny)</klr></trd>
  </trdgrp>
  <trd lng="nl">aardappel<klr tip="ind"> (plant)</klr></trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_t;&c_o;&c_f;&c_je;&c_l;&c_mol;<klr tip="amb"> (&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="tr">patates</trd>
</drv>

<drv mrk="ter.0pomfloko">
  <kap><tld/>pomfloko</kap>
  <fnt><bib>HejmVort</bib></fnt>
  <snc mrk="ter.0pomfloko.KUI">
    <uzo tip="fak">KUI</uzo>
    <ref tip="dif" cel="cxips.0o">&ccirc;ipso</ref>
  </snc>
  <trdgrp lng="en">
    <trd>chip<klr> (Usono)</klr></trd>,
    <trd>crisp<klr> (Britujo)</klr></trd>
  </trdgrp>
  <trd lng="de">Kartoffelflocke</trd>
  <trd lng="es">papas fritas<klr> (plural)</klr></trd>
  <trd lng="fr">chips</trd>
  <trdgrp lng="hu">
    <trd>burgonyaszirom</trd>,
    <trd>csipsz</trd>
  </trdgrp>
  <trd lng="nl">chip<klr tip="ind"> (aardappelchip)</klr></trd>
  <trd lng="ru">&c_ch;&c_i;&c_p;&c_s;&c_y;<klr> (plurale)</klr></trd>
</drv>

<drv mrk="ter.0pomfingro">
  <kap><tld/>pomfingro</kap>
  <fnt><bib>HejmVort</bib></fnt>
  <snc mrk="ter.0pomfingro.KUI">
    <uzo tip="fak">KUI</uzo>
    <ref tip="dif" cel="frit.0o">frito</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Pommes frites</trd>,
    <trd>Fritten</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>French <ind>fry</ind><klr> (Usono)</klr></trd>,
    <trd>potatoe <ind>chip</ind><klr> (Britujo)</klr></trd>
  </trdgrp>
  <trd lng="es">papas a la francesa<klr> (plural)</klr></trd>
  <trd lng="fr">frite</trd>
  <trdgrp lng="he">
    <trd>&tet;&vav;&gimel;&fnun;</trd>,
    <trd>&tsadi;'&yod;&pe;&samekh;<klr> (&alef;&het;&dalet;)</klr></trd>
  </trdgrp>
  <trd lng="hu">has&aacute;bburgonya</trd>
  <trdgrp lng="nl">
    <trd>friet</trd>,
    <trd>frites</trd>,
    <trd>patat<klr tip="ind"> (frites)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_r;&c_t;&c_o;&c_f;&c_je;&c_l;&c_mol; &c_v;&c_o; &c_f;&c_r;&c_i;&c_t;&c_ju;&c_r;&c_je;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_t;&c_o;&c_f;&c_je;&c_l;&c_mol; &c_zh;&c_a;&c_r;&c_je;&c_n;&c_y;&c_j; &c_s;&c_o;&c_l;&c_o;&c_m;&c_k;&c_o;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="ter.0skuo">
  <kap><tld/>skuo</kap>
  <snc mrk="ter.0skuo.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      &Ccirc;iu el pli-malpli fortaj <ref tip="vid"
      cel="sism.0a">sismaj</ref>
      movoj en <tld/>tremo.
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_t;&c_u;&c_r;&c_sh;&c_o;&c_k; <klr tip="amb">(&c_p;&c_a;&c_d;&c_ch;&c_a;&c_s; &c_z;&c_je;&c_m;&c_l;&c_ja;&c_t;&c_r;&c_u;&c_s;&c_a;)</klr></trd>
  <trd lng="fr">secousse tellurique</trd>
  <trd lng="hu">f&ouml;ldl&ouml;k&eacute;s</trd>
  <trd lng="nl">aardtrilling</trd>
  <trd lng="ru">&c_t;&c_o;&c_l;&c_ch;&c_o;&c_k;<klr tip="amb"> (&c_p;&c_r;&c_i; &c_z;&c_je;&c_m;&c_l;&c_je;&c_t;&c_r;&c_ja;&c_s;&c_je;&c_n;&c_i;&c_i;)</klr></trd>
</drv>

<drv mrk="ter.0tremo">
  <kap><tld/>tremo<fnt>Z</fnt></kap>
  <snc mrk="ter.0tremo.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Grava movo de la <tld/>krusto, ordinare okaziganta domfalojn ktp.:
      <ekz>
        fari&gcirc;is granda <tld/>tremo, tiel ke la fundamentoj de la
        malliberejo skui&gcirc;is
        <fnt><bib>NT</bib>, <lok>&Ago; 16:26</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_je;&c_m;&c_l;&c_ja;&c_t;&c_r;&c_u;&c_s;</trd>
  <trd lng="es">temblor</trd>
  <trdgrp lng="fr">
    <trd>s&eacute;isme</trd>,
    <trd>tremblement de terre</trd>
  </trdgrp>
  <trd lng="hu">f&ouml;ldreng&eacute;s</trd>
  <trd lng="nl">aardbeving</trd>
  <trd lng="ru">&c_z;&c_je;&c_m;&c_l;&c_je;&c_t;&c_r;&c_ja;&c_s;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="ter.0zono">
  <kap><tld/>zono</kap>
  <snc mrk="ter.0zono.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      &Ccirc;iu el la kvin grandaj dividoj de la <tld/>globo, inter
      la tropikoj kaj la polusoj:
      <ekz>
        varmega, mezvarma, glacia <tld/>zono.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_ib;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_ja; &c_z;&c_o;&c_n;&c_a;</trd>
  <trd lng="hu">&eacute;gt&aacute;j</trd>
  <trd lng="nl">aardgordel</trd>
  <trd lng="ru">&c_k;&c_l;&c_i;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; &c_p;&c_o;&c_ja;&c_s;</trd>
</drv>
