<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>ter</rad>/o
</kap>
<drv mrk="ter.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="ter.0o.planedo">
    <dif>
      La <ref tip="super" cel="planed.0o">planedo</ref>,
      sur kiu ni lo&gcirc;as.
    </dif>
    <ref tip="sin" cel="mond.0o.tero">mondo</ref>
    <trdgrp lng="tr">
      <trd>d&uuml;nya</trd>,
      <trd>Yer</trd>
    </trdgrp>
  </snc>
  <snc mrk="ter.0o.surfaco">
    <dif>
      Ekstera firma tavolo de tiu planedo:
      <ekz>
        ku&scirc;i sur la <tld/>o;
      </ekz>
      <ekz>
        la <tld/>o kaj la maro;
      </ekz>
      <ekz>
        super la <tld/>o sin trovas aero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        fosi la <tld/>on;
      </ekz>
      <ekz>
        la rajdanto desaltis <tld/>en;
      </ekz>
      <ekz>
        fali <tld/>en;
      </ekz>
      <ekz>
        ili &jcirc;eti&gcirc;is viza&gcirc;al<tld/>e<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="tr">
      <trd>yery&uuml;z&uuml;</trd>,
      <trd>d&uuml;nya</trd>
    </trdgrp>
  </snc>
  <snc mrk="ter.0o.substanco">
    <dif>
      Substanco el kiu konsistas tiu tavolo:
      <ekz>
        poto el <tld/>o;
      </ekz>
      <ekz>
        argila, sabla, seka, kulturebla <tld/>o
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="grund.0o">grundo</ref>.
    </refgrp>
    <trd lng="tr">toprak</trd>
  </snc>
  <trd lng="cs">zem</trd>
  <trd lng="de">Erde</trd>
  <trd lng="en">earth</trd>
  <trdgrp lng="nl">
    <trd>aarde</trd>,
    <trd>grond</trd>
  </trdgrp>
</drv>
<drv mrk="ter.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Apartenanta al <tld/>o 1:
      <ekz>
        la <tld/>a supra&jcirc;o;
      </ekz>
      <ekz>
        la <tld/>a diametro.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Konsistanta el <tld/>o 3:
      <ekz>
        <tld/>a poto.
      </ekz>
    </dif>
    <trd lng="de">irden</trd>
    <trd lng="nl">aard-</trd>
</snc>
</drv>
<drv mrk="ter.al0igxi">
  <kap>al<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Alveni al la <tld/>o 2.
    </dif>
  </snc>
    <trd lng="de">landen</trd>
    <trd lng="nl">landen</trd>
    </drv>
<drv mrk="ter.en0igi">
  <kap>en<tld/>igi</kap>
  <snc>
    <dif>
      Meti en la <tld/>on:
      <ekz>
        en<tld/>igi trezoron, markoton.
      </ekz>
    </dif>
    <trd lng="de">eingraben</trd>
    <trd lng="nl">ingraven</trd>
  </snc>
  <snc>
    <dif>
      Entombigi:
      <ekz>
        en<tld/>igi mortinton.
      </ekz>
    </dif>
    <trd lng="de">begraben</trd>
    <trd lng="nl">begraven</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Forgesigi, vanigi, neniigi:
      <ekz>
        tiu projekto estas jam tute
        senkreditigita kaj en<tld/>igita<fnt>Z</fnt>;
      </ekz>
      <ekz>
        eble nia afero &scirc;ajnos al la mondo e&ccirc; mortinta
        kaj jam en <tld/>igita<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">begraben</trd>
    <trd lng="nl">begraven</trd>
  </snc>
  <trd lng="fr">enterrer</trd>
</drv>
<drv mrk="ter.el0igi">
  <kap>el<tld/>igi</kap>
  <snc>
    <dif>
      Eltiri el la <tld/>o.
    </dif>
  </snc>
  <trd lng="de">ausgraben</trd>
  <trd lng="fr">d&eacute;terrer</trd>
  <trd lng="nl">opgraven</trd>
</drv>
<drv mrk="ter.sub0ajxo">
  <kap>sub<tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Galerio fosita sub la <tld/>o
    </dif>
    <refgrp tip="sin">
      <ref cel="tunel.0o">tunelo</ref>.
    </refgrp>
   <refgrp tip="vid">
      <ref cel="kel.0o">kelo</ref>.
   </refgrp></snc>
  <trd lng="fr">souterrain</trd>
  <trd lng="nl">onderaardse <ind>gang</ind></trd>
</drv>
<drv mrk="ter.sur0a">
  <kap>sur<tld/>a</kap>
  <snc>
    <dif>
      Okazanta a&ubreve; vivanta sur la <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">terrestre</trd>
  <trdgrp lng="nl">
    <trd>bovenaards</trd>,
    <trd>bovengronds</trd>
  </trdgrp>
</drv>
<drv mrk="ter.0etagxo">
  <kap><tld/>eta&gcirc;o</kap>
  <snc>
    <dif>
      Eta&gcirc;o samnivela kiel la &ccirc;irka&ubreve;anta grundo.
    </dif>
  </snc>
  <trd lng="de">Erdgeschoss</trd>
  <trd lng="fr">rez de chauss&eacute;e</trd>
  <trdgrp lng="nl">
    <trd>begane <ind>grond</ind></trd>,
    <trd>benedenverdieping</trd>
  </trdgrp>
<trd lng="tr">zemin kat</trd>
</drv>
<drv mrk="ter.0globo">
  <kap><tld/>globo</kap>
  <snc>
    <ref tip="dif" cel="ter.0o.planedo"><tld/>o
    <sncref ref="ter.0o.planedo"/></ref>
  </snc>
  <trd lng="de">Erdkugel</trd>
  <trd lng="nl">aardbol</trd>
</drv>
<drv mrk="ter.0kapo">
  <kap><tld/>kapo</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Parto de <tld/>o anta&ubreve;eni&gcirc;anta en maro
    </dif>
    <refgrp tip="sin">
      <ref cel="kab.0o">kabo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Kap</trd>
  <trd lng="fr">cap</trd>
  <trd lng="tr">burun</trd>
  <trd lng="nl">kaap</trd>
</drv>
<drv mrk="ter.0pomo">
  <kap><tld/>pomo</kap>
  <uzo tip="fak">BOT</uzo>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Man&gcirc;ebla tubero esti&gcirc;anta &ccirc;e la radikoj de
      <tld/>pomo 2.
    </dif>
  </snc>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="solan.0o">solano</ref>
      (<trd lng="la">Solanum tuberosum</trd>), devena de Ameriko
      kun man&gcirc;eblaj radiktuberoj.
    </dif>
  </snc>
  <trd lng="de">Kartoffel</trd>
  <trd lng="fr">pomme de terre</trd>
  <trd lng="nl">aardappel</trd>
  <trd lng="tr">patates</trd>
</drv>
<drv mrk="ter.0pomfloko">
  <kap><tld/>pomfloko</kap>
  <uzo tip="fak">KUI</uzo>
  <fnt><bib>HejmVort</bib></fnt>
  <snc>
    <dif>
       <ref tip="sin" cel="cxips.0o">&ccirc;ipso</ref>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>chip <klr>(Usono)</klr></trd>,
    <trd>crisp <klr>(Britujo)</klr></trd>
  </trdgrp>
  <trd lng="de">Kartoffelflokke</trd>
  <trd lng="fr">chips</trd>
  <trd lng="nl">chip</trd>
</drv>
<drv mrk="ter.0pomfingr">
  <kap><tld/>pomfingro, <tld/>pomfrito</kap>
  <uzo tip="fak">KUI</uzo>
  <fnt><bib>HejmVort</bib></fnt>
  <snc>
    <dif>
       <ref tip="sin" cel="frit.0o">frito</ref>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Pommes frites</trd>,
    <trd>Fritten</trd>
  </trdgrp>
  <trdgrp lng="en"><trd>French <ind>fry</ind><klr> (Usono)</klr></trd>,
    <trd>potatoe <ind>chip</ind><klr> (Britujo)</klr></trd></trdgrp>
  <trd lng="fr">pommes frites</trd>
  <trdgrp lng="nl">
    <trd>friet</trd>,
    <trd>frites</trd>,
    <trd>patat<klr> (Nederland)</klr></trd>
  </trdgrp>
</drv>
</art>
