<art mrk="$Id: fatamo.xml,v 1.16 2022/03/20 10:19:21 revo Exp $">
<kap>
  <rad>fatamorgan</rad>/o <fnt><bib>Fab4</bib></fnt> 
</kap>
<drv mrk="fatamo.0o">
  <kap><tld/>o</kap>
  <snc mrk="fatamo.0o.KOMUNE">
    <dif>
      <ref tip="dif" cel="miragx.0o">Mira&gcirc;o</ref>:
      <ekz>
        montri&gcirc;is la <tld lit="F"/>o, tiu belega aera fenomeno de la dezertoj, kun
        &scirc;vebantaj &gcirc;ardenoj kaj na&gcirc;antaj arbaroj, kaj samtempe anka&ubreve; tiu mirinda aera
        ondo
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        gapante al &scirc;i, kvaza&ubreve; al iu <tld/>o
        <fnt>
          <aut>J. Kozarac, trad. D. Klobu&ccaron;ar</aut>,
          <vrk><url ref="http://public.carnet.hr/esperanto-kel/slavonia_arbaro.pdf">
          Slavonia Arbaro</url></vrk>,
          <lok>Kroata Esperanto-Ligo, 2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_t;&c_a;-&c_m;&c_a;&c_r;&c_g;&c_a;&c_n;&c_a;</trd>,
    <trd>&c_m;&c_ib;&c_r;&c_a;&c_zh;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>fata morg&aacute;na</trd>,
    <trd>vidina</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Fata Morgana</trd>,
    <trd>Luftspiegelung</trd>
  </trdgrp>
  <trd lng="en">Fata Morgana</trd>
  <trd lng="fr">mirage</trd>
  <trd lng="hu">d&eacute;lib&aacute;b</trd>
  <trdgrp lng="nl">
    <trd>fata morgana</trd>,
    <trd>luchtspiegeling</trd>
  </trdgrp>
  <trd lng="pt">miragem</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_a;&c_t;&c_a;&c_m;&c_o;&c_r;&c_g;&c_a;&c_n;&c_a;</trd>,
    <trd>&c_m;&c_i;&c_r;&c_a;&c_zh;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mam</trd>,
    <trd>marivo</trd>,
    <trd>vidina</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x444;&#x430;&#x442;&#x430;-&#x43C;&#x43E;&#x440;&#x491;&#x430;&#x43D;&#x430;</trd>,
    <trd>&#x43C;&#x456;&#x440;&#x430;&#x436;</trd>,
    <trd>&#x43C;&#x430;&#x440;&#x435;&#x432;&#x43E;</trd>
  </trdgrp>
</drv>
</art>
