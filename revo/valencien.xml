<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: valencien.xml,v 1.7 2024/11/22 12:53:41 revo Exp $">
<kap>
  <rad>valencien</rad>/o
</kap>
<drv mrk="valencien.0o">
  <kap><tld/>o</kap>
  <snc mrk="valencien.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Grandma&scirc;a <ref tip="super" cel="punt.0o.TEKS">punto</ref> kun
      a&jcirc;uraj
      desegnoj, siatempe el samnoma urbo en norda Francujo:
      <ekz>
        <tld lit="V"/>oj, Alensonoj, Brugoj
        <fnt><bib>Marta</bib></fnt>...
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_l;&c_ja;&c_n;&c_s;&c_mol;&c_je;&c_n;</trd>
  <trd lng="ca">puntes de Valenciennes</trd>
  <trd lng="fr">valenciennes<klr tip="amb"> (dentelle)</klr></trd>
  <trd lng="hu">valenciennes-csipke</trd>
  <trd lng="pl">walansjenka</trd>
  <trd lng="pt">valenciana</trd>
  <trd lng="ru">&c_v;&c_a;&c_l;&c_a;&c_n;&c_s;&c_mol;&c_je;&c_n;<klr tip="amb"> (&c_k;&c_r;&c_u;&c_zh;&c_je;&c_v;&c_o;)</klr></trd>
</drv>
</art>

