<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: timbukt.xml,v 1.2 2022/03/28 14:44:11 revo Exp $">
    <kap>
      <rad>Timbuktu</rad>/o
    </kap>

<drv mrk="timbukt.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="timbukt.0o.oaza_urbo">
    <dif>
      Oaza <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      en <ref tip="malprt" cel="mali.M0o">Malio</ref> &ccirc;e la
      suda rando de <ref tip="malprt" cel="sahar.0o">Saharo</ref>
      (<frm>2&deg;39&#x2019;U, 16&deg;43&#x2019;N</frm>):
      <ekz>
        deputito de <tld/>o
        <fnt>
          <vrk><url ref="https://eo.mondediplo.com/article1906.html">Le Monde diplomatique' en Esperanto]</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;oja tago en Gao kaj en <tld/>o, post la unuaj armeaj sukcesoj
        <fnt>
          <vrk><url ref="https://eo.mondediplo.com/article2478.html">Le Monde diplomatique' en Esperanto]</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/d/dd/Djingareiber_cour.jpg/180px-Djingareiber_cour.jpg" prm="CC BY-SA 2.0 fr">Argila moskeo en <tld/>o
        <fnt>
          <aut>KaTeznik, propra verko</aut>
          <lok><url ref="&WCW;/File:Djingareiber_cour.jpg">https://commons.wikimedia.org/wiki/File:Djingareiber_cour.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Timbuktu</trd>
    <trd lng="en">Timbuktu</trd>
    <trd lng="fr">Tombouctou</trd>
  </snc>
  <trdgrp lng="uk">
    <trd>&#x422;&#x43E;&#x43C;&#x431;&#x443;&#x43A;&#x442;&#x443;</trd>,
    <trd>&#x422;&#x456;&#x43C;&#x431;&#x443;&#x43A;&#x442;&#x443;</trd>
  </trdgrp>
</drv>

  </art>

