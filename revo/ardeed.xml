<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ardeed.xml,v 1.8 2019/03/26 20:10:17 revo Exp $">
<kap>
  <rad>ardeed</rad>/oj
</kap>
<drv mrk="ardeed.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_familioj" cel="famili.0o.BIO">Familio</ref>
      el la ordo <ref tip="malprt" cel="bird.vad0oj">vadbirdoj</ref>
      (<trd lng="la">Ardeidae</trd>), de mar&ccirc;aj birdoj longkruraj, longkolaj kaj longbekaj:
      <ekz>
        la familio de <tld/>oj estas ampleksa familio de vadbirdoj
        <klr>[&#8230;]</klr>, kiu enhavas pli ol 60 speciojn, inter ili <ref tip="prt" cel="arde.0o.ZOO">ardeo</ref>jn,
        egretojn kaj <ref tip="prt" cel="botaux.0o">bota&ubreve;ro</ref>jn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=55524"
              >Ardeedoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_p;&c_l;&c_ib; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">ardeids</trd>
  <trd lng="de">Reiher <klr>(Familie)</klr></trd>
  <trd lng="en">herons</trd>
  <trd lng="fr">ard&eacute;id&eacute;s</trd>
  <trd lng="hu">g&eacute;mf&eacute;l&eacute;k</trd>
  <trdgrp lng="ru">
    <trd><ind>&c_c;&c_a;&c_p;&c_l;&c_i;</ind> &c_n;&c_a;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_i;&c_je;</trd>,
    <trd>&c_c;&c_a;&c_p;&c_l;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
</drv>
</art>

