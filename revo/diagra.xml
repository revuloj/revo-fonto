    <trd>diagrama de past&iacute;s</trd>,
    <trd>gr&agrave;fic circular</trd>,
    <trd>gr&agrave;fic de sectors</trd>,
    <trd>gr&agrave;fic de past&iacute;s</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kreisdiagramm</trd>,
    <trd>Tortendiagramm</trd>
  </trdgrp>
