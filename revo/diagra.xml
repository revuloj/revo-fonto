<drv mrk="diagra.are0o">
  <kap>are<tld/>o,
  <var><kap>areo<tld/>o</kap></var>,
  <var><kap>area <tld/>o</kap></var></kap>
  <snc mrk="diagra.are0o.SCI">
    <dif>
      Dudimensia <ref tip="super" cel="diagra.0o.SCI"><tld/>o</ref> kiu prezentas kvantojn kiel
      senrompajn surfacojn pli a&ubreve; malpli altajn:
      <ekz>
        areo<tld/>o kun pluraj datumaj serioj
        <fnt>
          <aut>-</aut>
          <vrk><url ref="https://eo.wikipedia.org/wiki/%C5%9Cablono:Graph:Chart">
          &Scirc;ablono:Graph:Chart</url></vrk>
          <lok>Vikipedio, 2022-04-20</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>diagrama d'&agrave;rees</trd>,
    <trd>gr&agrave;fic d'&agrave;rees</trd>
  </trdgrp>
  <trd lng="de">Fl&auml;chendiagramm</trd>
  <trd lng="en">area chart</trd>
  <trd lng="es">gr&aacute;fico de &aacute;reas</trd>
  <trd lng="fr">graphique en aires</trd>
  <trd lng="nl">vlakdiagram</trd>
</drv>

<drv mrk="diagra.bret0o">
  <kap>bret<tld/>o,
  <var><kap>bastona <tld/>o <fnt><bib>Komputeko</bib></fnt></kap></var></kap>
  <snc mrk="diagra.bret0o.SCI">
    <dif>
      Dudimensia <ref tip="super" cel="diagra.0o.SCI"><tld/>o</ref> kiu prezentas kvantojn kiel
      ku&scirc;ajn, diverse lar&gcirc;ajn ortangulojn:
      <ekz>
        duoble alklaku unuopan datumpunkton por redakti la atributojn de tiu punkto, ekzemple,
        unuopan breton en bret<tld/>o
        <fnt>
          <aut>-</aut>
          <vrk><url ref="https://help.libreoffice.org/6.0/eo/text/schart/main0000.html">
          Uzado de diagramoj en LibreOffice</url></vrk>
          <lok>Helpo de LibreOffice 6.0, [vidita en 2025]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>diagrama de barres</trd>,
    <trd>gr&agrave;fic de barres</trd>
  </trdgrp>
  <trd lng="de">Balkendiagramm</trd>
  <trd lng="en">bar chart</trd>
  <trd lng="es">gr&aacute;fico de barras</trd>
  <trd lng="fr">graphique &agrave; barres horizontales</trd>
  <trd lng="nl">staafdiagram</trd>
</drv>

<drv mrk="diagra.kolumno0o">
  <kap>kolumno<tld/>o,
  <var><kap>kolumna <tld/>o</kap></var></kap>
  <snc mrk="diagra.kolumno0o.SCI">
    <dif>
      Dudimensia <ref tip="super" cel="diagra.0o.SCI"><tld/>o</ref> kiu prezentas kvantojn kiel
      starajn, diverse altajn ortangulojn:
      <ekz>
        tiu montras helpe de kolumna <tld/>o tempon, kiam la mesa&gcirc;oj alvenis
        <fnt>
          <aut>M. Noska, trad. Katjo</aut>
          <vrk><url ref="https://ikso.net/thunderbird-30-venas-kun-paneloj-kaj-novaj-sercx-ebloj/">
          Thunderbird 3.0 venas kun paneloj kaj novaj ser&ccirc;ebloj</url></vrk>
          <lok>E@I, 2009-12-11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="histogram.0o">histogramo</ref>
  </refgrp>
  <trd lng="be">&c_s;&c_l;&c_u;&c_p;&c_k;&c_a;&c_v;&c_a;&c_t;&c_a;&c_ja;
  <ind>&c_d;&c_y;&c_ja;&c_g;&c_r;&c_a;&c_m;&c_a;</ind></trd>
  <trdgrp lng="ca">
    <trd>diagrama de barres verticals</trd>,
    <trd>gr&agrave;fic de barres verticals</trd>
  </trdgrp>
  <trd lng="de">S&auml;ulendiagramm</trd>
  <trd lng="en">column chart</trd>
  <trd lng="es">gr&aacute;fico de columnas</trd>
  <trd lng="fr">graphique &agrave; barres verticales</trd>
  <trd lng="it">diagramma a colonne</trd>
  <trd lng="nl">kolomdiagram</trd>
</drv>

<drv mrk="diagra.lini0o">
