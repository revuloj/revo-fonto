<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>gavot</rad>/o
<fnt>Z</fnt></kap>
<drv mrk="gavot.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="gavot.0o.danco">
    <dif>
      E&ubreve;ropa <ref tip="super" cel="danc.0o">danco</ref> franc-devena,
      du-takta, rapida, populara fine de la 16a jarcento.
    </dif>
  </snc>
  <snc mrk="gavot.0o.muzikajxo">
    <dif>
      Muzika&jcirc;o ta&ubreve;ga por <tld/>-danco, uzata ekde la 17a jarcento
      kiel parto de <ref tip="malprt" cel="suit.0o">suito</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_v;&c_o;&c_t;</trd>
  <trd lng="bg">&c_g;&c_a;&c_v;&c_o;&c_t;</trd>
  <trd lng="ca">gavota</trd>
  <trd lng="de">Gavotte</trd>
  <trd lng="en">gavotte</trd>
  <trd lng="fr">gavotte</trd>
  <trd lng="hu">gavotte</trd>
  <trd lng="it">gavotta</trd>
  <trd lng="nl">gavotte</trd>
  <trd lng="pt">gavota</trd>
  <trd lng="ru">&c_g;&c_a;&c_v;&c_o;&c_t;</trd>
  <trd lng="sk">gavota</trd>
</drv>
</art>
