<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>centav</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="centav.0o">
<kap><tld/>o</kap>
<uzo tip="fak">EKON</uzo>
<snc>
<dif>
<ref tip="super" cel="unu.monasub0o">Mona subunuo</ref>, egala al 
centono de <ref tip="malprt" cel="pes1.0o">peso</ref>;
tiomvalora <ref tip="super" cel="mon.0ero">monero</ref>.
</dif>
</snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_e;&c_n;&c_t;&c_a;&c_v;&c_a;</trd>,
    <trd>&c_s;&c_e;&c_n;&c_t;&c_a;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Centavo</trd>
  <trd lng="el">&lambda;&epsilon;&pi;&tau;&omikron_ton;</trd>
  <trd lng="es">centavo</trd>
  <trd lng="fr">centavo</trd>
  <trd lng="hu">centavo</trd>
  <trd lng="nl">centavo</trd>
  <trd lng="pl">centavo</trd>
  <trd lng="pt">centavo</trd>
  <trdgrp lng="ru">
    <trd>&c_c;&c_je;&c_n;&c_t;&c_a;&c_v;&c_o;</trd>,
    <trd>&c_s;&c_je;&c_n;&c_t;&c_a;&c_v;&c_o;</trd>
  </trdgrp>
</drv>
</art>
