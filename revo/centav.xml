<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: centav.xml,v 1.22 2023/08/31 10:10:26 revo Exp $">
<kap>
  <rad>centav</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="centav.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="unu.monasub0o" lst="voko:monunuoj">Mona
      subunuo</ref>, egala al centono de <ref tip="malprt" cel="pes1.0o">peso</ref> k.a.;
      <ref tip="super" cel="mon.0ero">monero</ref> de tiu valoro:
      <ekz>
        Meksika peso <klr>[&#x2026;]</klr> dividi&gcirc;as en 100 <tld/>ojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=295050">Meksika peso</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        unu metikalo estas dividita je cent <tld/>oj, kvankam praktike pro inflacio
        <tld/>oj ne plu uzi&gcirc;as
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=340137">Mozambika metikalo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_e;&c_n;&c_t;&c_a;&c_v;&c_a;</trd>,
    <trd>&c_s;&c_e;&c_n;&c_t;&c_a;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">centau</trd>
  <trd lng="cs">centavo</trd>
  <trd lng="de">Centavo</trd>
  <trd lng="el">&lambda;&epsilon;&pi;&tau;&omikron_ton;</trd>
  <trd lng="es">centavo</trd>
  <trd lng="fr">centavo</trd>
  <trd lng="hu">centavo</trd>
  <trd lng="it">centesimo<klr> (es: di peseta)</klr></trd>
  <trd lng="ja">&#x30BB;&#x30F3;&#x30BF;&#x30DC;</trd>
  <trd lng="nl">centavo</trd>
  <trd lng="pl">centavo</trd>
  <trd lng="pt">centavo</trd>
  <trdgrp lng="ru">
    <trd>&c_c;&c_je;&c_n;&c_t;&c_a;&c_v;&c_o;</trd>,
    <trd>&c_s;&c_je;&c_n;&c_t;&c_a;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">centavo</trd>
  <trd lng="uk">&#x441;&#x435;&#x43D;&#x442;&#x430;&#x432;&#x43E;</trd>
</drv>
</art>
