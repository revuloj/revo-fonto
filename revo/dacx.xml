<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: dacx.xml,v 1.2 2021/09/17 11:26:05 afido Exp $">
    <kap>
      <rad>da&ccirc;</rad>/o
    </kap>

<drv mrk="dacx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ARKI</uzo>
  <snc mrk="dacx.0o.rusa">
    <dif>
      Rusa <ref tip="super" cel="dom.somer0o">somerdomo</ref>:
      <ekz>
        agentoj de <nom>NKVD</nom> marodas la <tld/>on kaj likvidas atestantojn
        <fnt>
          <vrk><url ref="http://www.plansprachen.ch/Morto_de_Stalin.pdf">La morto de
          Stalin</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ni ekiras direkte al Flughaveno &Scirc;eremetjevo <klr>[&#8230;]</klr> kaj al la <tld/>o
        kiun la 40-jarulo estas ekipinta kiel konstanta hejmo
        <fnt>
          <bib>MD14</bib>
          <vrk>Unu el la plej obstrukcitaj urboj de la mondo</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="alberg.0o">albergo</ref>,
       <ref cel="cxaled.0o">&ccirc;aledo</ref>,
       <ref cel="kaban.0o">kabano</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_d;&c_a;&c_ch;&c_a;</trd>,
      <trd>&c_l;&c_je;&c_c;&c_ib;&c_sh;&c_ch;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Datsche</trd>,
      <trd>Bungalow</trd>,
      <trd>Gartenhaus</trd>,
    </trdgrp>
    <trd lng="en">dacha</trd>
    <trd lng="fr">datcha</trd>
    <trd lng="ru">&c_d;&c_a;&c_ch;&c_a;</trd>
  </snc>
</drv>

  </art>

