<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>fontan</rad>/o
</kap>
<drv mrk="fontan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Loko a&ubreve; konstrua&jcirc;o, el kiu artefare elfluas akvo:
      <ekz>per statuoj ornamita <tld/>o.</ekz>
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Konstrua&jcirc;o kun artefarita tubaran&gcirc;o, kiu alten &scirc;prucigas akvon.  
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Springbrunnen</trd>,
     <trd>Font&auml;ne</trd></trdgrp>
  <trd lng="es">fuente</trd>
  <trd lng="fr">fontaine</trd>
  <trd lng="ru">&c_f;&c_o;&c_n;&c_t;&c_a;&c_n;</trd>
</drv>
</art>
