<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: spasm.xml,v 1.23 2025/10/09 18:39:34 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>spasm</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="spasm.0o">
  <kap><tld/>o</kap>
  <snc mrk="spasm.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Nevola forta <ref tip="super" cel="strecx.0o">stre&ccirc;o</ref> de la muskoloj:
      <ekz>
        &scirc;iajn membrojn agitis konvulsiaj <tld/>oj
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la infana koreto batadis per forto <tld/>a kaj
        dis&scirc;iranta
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        histeria <tld/>o kaptas &scirc;ian gor&gcirc;on
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de la pasia deziro kunbuligis &scirc;ian korpon
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o tordis lian viza&gcirc;on
        <fnt>
          <bib>MkM</bib>
          <lok>5</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ajnis al mi, ke mi krias, sufokita per <tld/>o de senlima timo
        <fnt>
          <bib>FdO</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konvul.0o">konvulsio</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_a;&c_z;&c_m;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_t;&c_a;&c_r;&c_g;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>k&rcaron;e&ccaron;</trd>,
    <trd>spazmus</trd>
  </trdgrp>
  <trd lng="de">Krampf</trd>
  <trd lng="en">spasm</trd>
  <trd lng="fr">spasme</trd>
  <trd lng="hu">g&ouml;rcs</trd>
  <trdgrp lng="ja">
    <trd>&#x3051;&#x3044;&#x308C;&#x3093;</trd>,
    <trd>&#x5F15;&#x304D;&#x3064;&#x308A;  <pr>&#x3072;&#x304D;&#x3064;&#x308A;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kramp</trd>,
    <trd>spasme</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>skurcz</trd>,
    <trd>konwulsja</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_a;&c_z;&c_m;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_z;&c_m;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_d;&c_o;&c_r;&c_o;&c_g;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">k&racute;&ccaron;</trd>
  <trd lng="sv">spasm</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_p;&c_a;&c_z;&c_m;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_d;&c_o;&c_m;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_d;&c_o;&c_r;&c_o;&c_g;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_ch;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x75C9; <pr>j&igrave;ng</pr></trd>,
    <trd>&#x75D9; <pr>j&igrave;ng</pr></trd>,
    <trd>&#x75C9;&#x631B; <pr>j&igrave;nglu&aacute;n</pr></trd>,
    <trd>&#x75D9;&#x6523; <pr>j&igrave;nglu&aacute;n</pr></trd>,
    <trd>&#x62BD;&#x6410; <pr>ch&omacron;uch&ugrave;</pr></trd>,
    <trd>&#x62BD;&#x98A8; <pr>ch&omacron;uf&emacron;ng</pr></trd>,
    <trd>&#x62BD;&#x98CE; <pr>ch&omacron;uf&emacron;ng</pr></trd>,
    <trd>&#x761B; <pr>ch&igrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="spasm.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="spasm.0i.MED">
    <uzo tip="fak">MED</uzo>
    <uzo tip="klr">(pri muskolo, korpoparto)</uzo>
    <dif>
      Subite, nevole ektiri&gcirc;i:
      <ekz>
        mi sentis pulson en mia frunto, kiu <tld/>is samtakte
        <fnt>
          <bib>StU</bib>
          <lok>Britujo</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj lipoj <tld/>is dekstren kaj maldekstren,
        eligante ian sonon, senkoheran kiel murmuro
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_o;&c_r;&c_ch;&c_y;&c_c;&c_c;&c_a; &c_w; <ind>&c_s;&c_u;&c_t;&c_a;&c_r;&c_g;&c_a;&c_h;</ind></trd>,
    <trd>&c_a;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_c;&c_mol; <ind>&c_s;&c_p;&c_a;&c_z;&c_m;&c_y;</ind></trd>
  </trdgrp>
  <trd lng="cs">m&iacute;t k&rcaron;e&ccaron;</trd>
  <trdgrp lng="de">
    <trd>krampfen</trd>,
    <trd>sich <ind>zusammenziehen</ind></trd>
  </trdgrp>
  <trd lng="en">spasm</trd>
  <trdgrp lng="fr">
    <trd>se <ind>convulser</ind></trd>,
    <trd>se <ind>crisper</ind></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3051;&#x3044;&#x308C;&#x3093;&#x3092;&#x8D77;&#x3053;&#x3059;  <pr>&#x3051;&#x3044;&#x308C;&#x3093;&#x3092;&#x304A;&#x3053;&#x3059;</pr></trd>,
    <trd>&#x5F15;&#x304D;&#x3064;&#x308B;  <pr>&#x3072;&#x304D;&#x3064;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>spazmowa&cacute;</trd>,
    <trd>mie&cacute;  <ind>skurcze</ind></trd>
  </trdgrp>
  <trd lng="sk">ma&tcaron; k&racute;&ccaron;</trd>
</drv>
</art>
