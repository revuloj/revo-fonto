<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: spasm.xml,v 1.19 2023/10/21 07:46:48 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>spasm</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="spasm.0o">
  <kap><tld/>o</kap>
  <snc mrk="spasm.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Nevola forta stre&ccirc;o de la muskoloj:
      <ekz>
        la infana koreto batadis per forto <tld/>a kaj
        dis&scirc;iranta<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konvul.0o">konvulsio</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_a;&c_z;&c_m;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_t;&c_a;&c_r;&c_g;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>k&rcaron;e&#x10D;</trd>,
    <trd>spazmus</trd>
  </trdgrp>
  <trd lng="de">Krampf</trd>
  <trd lng="en">spasmo</trd>
  <trd lng="fr">spasme</trd>
  <trd lng="hu">g&ouml;rcs</trd>
  <trdgrp lng="ja">
    <trd>&#12369;&#12356;&#x308C;&#12435;</trd>,
    <trd>&#x5F15;&#x304D;&#12388;&#x308A;  <pr>&#12402;&#x304D;&#12388;&#x308A;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kramp</trd>,
    <trd>spasme</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>skurcz</trd>,
    <trd>konwulsja</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_a;&c_z;&c_m;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_z;&c_m;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_d;&c_o;&c_r;&c_o;&c_g;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">k&racute;&#x10D;</trd>
  <trd lng="sv">spasm</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43F;&c_a;&c_z;&#x43C;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_d;&#x43E;&#x43C;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_d;&#x43E;&c_r;&#x43E;&c_g;&c_a;</trd>,
    <trd>&#x43A;&#x43E;&c_r;&c_ch;&c_ib;</trd>
  </trdgrp>
</drv>

<drv mrk="spasm.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="spasm.0i.MED">
    <uzo tip="fak">MED</uzo>
    <uzo tip="klr">(pri muskolo, korpoparto)</uzo>
    <dif>
      Subite, nevole ektiri&gcirc;i:
      <ekz>
        liaj lipoj <tld/>is dekstren kaj maldekstren,
        eligante ian sonon, senkoheran kiel murmuro
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_o;&c_r;&c_ch;&c_y;&c_c;&c_c;&c_a; &c_w; <ind>&c_s;&c_u;&c_t;&c_a;&c_r;&c_g;&c_a;&c_h;</ind></trd>,
    <trd>&c_a;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_c;&c_mol; <ind>&c_s;&c_p;&c_a;&c_z;&c_m;&c_y;</ind></trd>
  </trdgrp>
  <trd lng="cs">m&#xED;t k&rcaron;e&#x10D;</trd>
  <trdgrp lng="fr">
    <trd>se <ind>convulser</ind></trd>,
    <trd>se <ind>crisper</ind></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12369;&#12356;&#x308C;&#12435;&#12434;&#x8D77;&#12371;&#12377;  <pr>&#12369;&#12356;&#x308C;&#12435;&#12434;&#x304A;&#12371;&#12377;</pr></trd>,
    <trd>&#x5F15;&#x304D;&#12388;&#x308B;  <pr>&#12402;&#x304D;&#12388;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>spazmowa&cacute;</trd>,
    <trd>mie&cacute;  <ind>skurcze</ind></trd>
  </trdgrp>
  <trd lng="sk">ma&tcaron; k&racute;&#x10D;</trd>
</drv>
</art>
