<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>apolog</rad>/o
</kap>
<drv mrk="apolog.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELE</uzo>
  <snc>
    <dif>
      Proza <ref tip="super" cel="fabl.0o">fablo</ref>, enmetita en
      rakonton, paroladon ks, por ilustri demonstradon.
    </dif>
  </snc>
  <trd lng="ca">ap&ograve;leg</trd>
  <trdgrp lng="de">
     <trd>Apolog</trd>,
     <trd>Gleichnis</trd>,
     <trd>Lehrfabel</trd>
  </trdgrp>
  <trd lng="en">apologue</trd>
  <trd lng="fr">apologue</trd>
  <trd lng="hu">tanmese</trd>
  <trd lng="lat">apologus, i</trd>
  <trd lng="nl">apoloog</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_t;&c_ch;&c_a;</trd>,
    <trd>&c_a;&c_p;&c_o;&c_l;&c_o;&c_g;</trd>
  </trdgrp>
  <trd lng="sv">fabel</trd>
</drv>
</art>
