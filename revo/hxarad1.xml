<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hxarad1.xml,v 1.14 2019/03/05 18:10:26 revo Exp $">
<kap>
   <rad>&hcirc;aradried</rad>/oj
</kap>
<drv mrk="hxarad1.0oj">
   <kap>
     <tld/>oj
   </kap>
   <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="hxarad.0oformaj">&hcirc;aradrioformaj
      birdoj</ref> (<trd lng="la">Charadriidae</trd>):
      <ekz>
        <tld/>oj <klr>[&#8230;]</klr> estas apudakvaj birdoj &ccirc;u malgrandaj &ccirc;u mezgrandaj, de
        rekta korposinteno, grandaj okuloj rondformaj kapoj kaj nelongaj bekoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=22524"
              >&Hcirc;aradriedoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="hxarad.0o">&hcirc;aradrio</ref>
  </snc>
   <trd lng="be">&c_s;&c_ja;&c_w;&c_c;&c_o;&c_v;&c_y;&c_ja; <klr
tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
   <trd lng="ca">car&agrave;drids</trd>
   <trd lng="de">Regenpfeiffer<klr tip="ind"> (Familie)</klr></trd>
   <trd lng="en">pluviers</trd>
   <trd lng="fi">kurmitsat</trd>
   <trd lng="fr">charadriid&eacute;s</trd>
   <trd lng="hu">lilef&eacute;l&eacute;k</trd>
   <trd lng="nl">plevieren</trd>
   <trd lng="pl">siewkowate</trd>
   <trd lng="pt">caradri&iacute;deos</trd>
   <trdgrp lng="ru">
     <trd>&c_r;&c_zh;&c_a;&c_n;&c_k;&c_i;<klr tip="ind">
(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
     <trd>&c_r;&c_zh;&c_a;&c_n;&c_k;&c_o;&c_v;&c_y;&c_je; 
<klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
   </trdgrp>
   <trd lng="it">caradriidi</trd>
</drv>
</art>

