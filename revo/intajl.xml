<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: intajl.xml,v 1.17 2023/10/17 16:46:14 revo Exp $">
<kap>
  <rad>intajl</rad>/o
</kap>
<drv mrk="intajl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      <ref cel="glipti.0o">Gliptika&jcirc;o</ref> kun konkava gravuro; 
      <ref tip="sin" cel="gem.0o.intajlo">gemo<sncref/></ref>:
      <ekz>
        ametisto<klr>[n oni]</klr> uzas <klr>[&#x2026;]</klr> ekzemple por <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=157333">Ametisto</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        en iuj komercaj lokoj oni faris ovalan moneron el bronzo kun <tld/>o sur la
        fronta flanko, la ovala monero kun <tld/>a&jcirc;oj kaj truoj impresas kiel stranga
        viza&gcirc;o, tial oni nomas &gcirc;in anka&ubreve; monero de diabla viza&gcirc;o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=52624">&Ccirc;ina monero</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="glipti.0o">gliptiko</ref>,
      <ref cel="tajl">(en)tajlo</ref>,
      <ref cel="kame.0o">kameo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_n;&c_t;&c_a;&c_l;&c_ib;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_n;&c_y; <ind>&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;</ind></trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_d;&c_malmol;&c_l;&c_b;&c_o;&c_k; &c_p;&c_je;&c_ch;&c_a;&c_t;</trd>,
    <trd>&c_g;&c_je;&c_m;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Intaglio</trd>,
    <trd>Gemme mit vertieft eingeschnittenem Bild</trd>
  </trdgrp>
  <trd lng="en">intaglio</trd>
  <trd lng="fr">intaille</trd>
  <trd lng="hu">intaglio</trd>
  <trd lng="ja">&#x6C88;&#x307F;&#x5F6B;&#x308A;  <pr>&#x3057;&#x305A;&#x307F;&#x307B;&#x308A;</pr></trd>
  <trd lng="nl">intaglio</trd>
  <trd lng="pl">intaglio</trd>
  <trd lng="pt">entalho<klr tip="amb"> (pe&ccedil;a entalhada)</klr>
  </trd>
  <trd lng="uk">&c_ib;&c_n;&c_t;&c_a;&c_l;&c_ib;&c_ja;</trd>
</drv>
</art>
