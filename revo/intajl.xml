<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>intajl</rad>/o
</kap>
<drv mrk="intajl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Gliptika&jcirc;o kun konkava gravuro.
    </dif>
    <refgrp tip="vid">
      <ref cel="glipti.0o">gliptiko</ref>,
      <ref cel="tajl">(en)tajlo</ref>,
      <ref cel="kame.0o">kameo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Intaglio</trd>,
    <trd>Gemme mit vertieft eingeschnittenem Bild</trd>
  </trdgrp>
  <trd lng="fr">intaille</trd>
  <trd lng="hu">intaglio</trd>
  <trd lng="nl">intaglio</trd>
  <trd lng="pt">entalho<klr tip="amb"> (pe&ccedil;a entalhada)</klr></trd>
</drv>
</art>
