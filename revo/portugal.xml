<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>portugal</rad>/o
  <ofc>*</ofc>
</kap>

<drv mrk="portugal.0ujo">
  <kap>
    <tld lit="P"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="portugal.0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la sudokcidenta <ref tip="malprt"
      cel="euxrop1.E0o.geog">E&ubreve;ropo</ref>
      (en Ibera Duoninsulo) kun &ccirc;efurbo Lisbono.
      Teritorio de Respubliko Portugalujo.
      ISO-3166 kodo: PT.
      &Gcirc;i limas kun
      <ref tip="vid" cel="hispan.H0ujo">Hispanujo</ref>
      kaj <ref tip="vid" cel="atlant.0o.GEOG">Atlantiko</ref>.
    </dif>
    <ref tip="sin" cel="portugali.P0o">Portugalio</ref>
    <trd lng="bg">&c_P;&c_o;&c_r;&c_t;&c_u;&c_g;&c_a;&c_l;&c_i;&c_ja;</trd>
    <trd lng="de">Portugal</trd>    
    <trd lng="en">Portugal</trd>
    <trdgrp lng="pl">
      <trd>Portugalia</trd>,
      <trd>Republika <ind>Portugalska</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

