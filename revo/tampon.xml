<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tampon</rad>/o
</kap>
<drv mrk="tampon.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      &Scirc;topilo el mola materialo por haltigi sangoelfluon el vundo
      ktp.
    </dif>
    <trd lng="ca">tamp&oacute;</trd>
    <trd lng="de">Mullbinde</trd>
    <trd lng="hu">tampon</trd>
    <trdgrp lng="nl">
      <trd>tampon</trd>,
      <trd>verband</trd>,
      <trd>drukverband</trd>
    </trdgrp>
  <trdgrp lng="pt">
   <trd>absorvente</trd>,
    <trd>absorvente &iacute;ntimo</trd>
   <trd>absorvente higi&ecirc;nico</trd>
  </trdgrp>
  </snc>
  <snc>
    <dif>
      &Scirc;topilo uzata por reteni menstruan sangon.
    </dif>
    <trd lng="de">Tampon</trd>
    <trd lng="hu">bet&eacute;t<klr tip="ind">
    (menstru&aacute;ci&oacute;s)</klr></trd>
    <trdgrp lng="nl">
      <trd>tampon</trd>,
      <trd>maandverband</trd>
    </trdgrp>
    <trd lng="sv">tampong</trd>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_m;&c_p;&c_o;&c_n;</trd>
  <trd lng="ca">tamp&oacute;</trd>
  <trd lng="de">Binde</trd>
  <trd lng="en">tampon</trd>
  <trd lng="es">tamp&oacute;n</trd>
  <trd lng="fr">tampon</trd>
  <trd lng="pl">tampon</trd>
  <trd lng="ru">&c_t;&c_a;&c_m;&c_p;&c_o;&c_n;</trd>
</drv>
</art>
