<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tampon</rad>/o
</kap>
<drv mrk="tampon.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      &Scirc;topilo el mola materialo por haltigi sangoelfluon el vundo ktp.
    </dif>
    <trd lng="de">Mullbinde</trd>
    <trdgrp lng="nl">
      <trd>tampon</trd>,
      <trd>verband</trd>,
      <trd>drukverband</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      &Scirc;topilo uzata por reteni menstruan sangon.
    </dif>
    <trd lng="de">Tampon</trd>
    <trdgrp lng="nl">
      <trd>tampon</trd>,
      <trd>maandverband</trd>
    </trdgrp>
    <trd lng="sv">tampong</trd>
  </snc>
  <trd lng="en">tampon</trd>
  <trd lng="fr">tampon</trd>
  <trd lng="ru">&c_t;&c_a;&c_m;&c_p;&c_o;&c_n;</trd>
</drv>
</art>
