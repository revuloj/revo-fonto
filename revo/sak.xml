  <trd lng="bg">&c_r;&c_a;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="bo">&t_ra;&t_gata;&t_yata;&t_ba;&t_tsheg;&t_pha;&t_da;&t_tsheg;</trd>
