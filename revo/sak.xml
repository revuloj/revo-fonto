<drv mrk="sak.du0o">
  <kap>du<tld/>o</kap>
  <snc mrk="sak.du0o.KOMUNE">
  <dif>
    Longa
    <ref tip="super" cel="sak.0o.KOMUNE"><tld/>o</ref>
    (pli&gcirc;uste, ta&scirc;o) fermita &ccirc;e amba&ubreve;
    ekstrema&jcirc;oj, kun malfermo en la mezo:
      <ekz>
        la biciklo impresis min pleje <klr>[&#8230;]</klr> pro la tol-du<tld/>o triangula,
        eluzita kaj pendanta sur la supra bicikloframo
        <fnt>
          <bib>Monato</bib>
          <aut>Wei Yubin</aut>
          <vrk><url ref="&Monato;/2012/010573.php"
              >La biciklo de mia patro</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">dvojpytel</trd>
  <trdgrp lng="es">
    <trd>alforjas</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>besace</trd>,
    <trd>bissac</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&aacute;ltalvet&odblac;</trd>,
    <trd>&aacute;talvet&odblac;</trd>
  </trdgrp>
  <trd lng="it">bisaccia</trd>
  <trd lng="nl">plunjezak</trd>
  <trd lng="pl">sakwa podw&oacute;jna</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_k;&c_i;&c_d;&c_n;&c_a;&c_ja; &c_s;&c_u;&c_m;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">dvojvrece</trd>
  <trdgrp lng="uk">
