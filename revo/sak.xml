<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sak</rad>/o
</kap>
<drv mrk="sak.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Granda, proksimume cilindroforma ujo el &scirc;nurteksa&jcirc;o a&ubreve; maldelikata
      &scirc;tofo, kun malferma&jcirc;o &ccirc;e la supro, por enteni grenon, farunon,
      karbon, ktp:
      <ekz>
        se <tld/>o tro pleni&gcirc;as, &gcirc;i balda&ubreve; dis&scirc;iri&gcirc;as
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Sack</trd>
    <trdgrp lng="fr">
      <trd>sac<klr> (de toile)</klr></trd>
    </trdgrp>
    <trd lng="hu">zs&aacute;k</trd>    
    <trd lng="ru">&c_m;&c_je;&c_sh;&c_o;&c_k;</trd>
  </snc>
  <snc>
    <dif>
      Malgranda diversforma ujo el &scirc;tofo, ledo, papero kc, por enteni
      iajn a&jcirc;ojn.
    </dif>
    <refgrp tip="vid">
      <ref cel="fel.0o">felo</ref>
    </refgrp>
    <trd lng="de">Sack</trd>
    <trdgrp lng="fr">
      <trd>sac</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>t&aacute;ska</trd>,
      <trd>sz&aacute;k</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_u;&c_m;&c_k;&c_a;</trd>,
      <trd>&c_s;&c_u;&c_m;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sak.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Monujo:
      <ekz>
        kiam <tld/>o mizeras, amo malaperas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        vortoj <tld/>on ne plenigas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        edzi&gcirc;o pro amo flamanta al <tld/>o sonanta<fnt>Z</fnt>.
      </ekz>
      <trdgrp lng="fr">
        <trd>bourse<klr> (sac d'argent)</klr></trd>
      </trdgrp>
      <trd lng="hu">p&eacute;nzeszs&aacute;k</trd>
      <trdgrp lng="ru">
        <trd>&c_k;&c_o;&c_sh;&c_je;&c_l;&c_jo;&c_k;</trd>,
        <trd>&c_m;&c_o;&c_sh;&c_n;&c_a;</trd>
      </trdgrp>
    </dif>
    <refgrp tip="vid">
      <ref cel="kas.0o">kaso</ref>,
      <ref cel="kest.0o">kesto</ref>,
      <ref cel="posx.0o">po&scirc;o</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Geldsack</trd>,
      <trd>Geldbeutel</trd>,
      <trd>Geldkatze</trd>,
      <trd>Portemonnaie</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">pytel</trd>
  <trd lng="en">sack</trd>
  <trd lng="nl">zak</trd>
</drv>

<drv mrk="sak.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Maldelikata pentovesto:
      <ekz>
        vi deprenis de mi mian <tld/>a&jcirc;on kaj zonis min per
        &gcirc;ojo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>sac<klr> (bibl. v&ecirc;tement de p&eacute;nitence)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_l;&c_a;&c_s;&c_ja;&c_n;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="sak.dormo0o">
  <kap>dormo<tld/>o</kap>
  <snc>
    <dif>
      Remburita kovrilo por endormo, kun unu nefermita ekstremo kapoflanka,
      kunportata kiel porvoja&gcirc;a lita&jcirc;o:
      <ekz>
        ni ja kunportis dormo<tld/>ojn
        <fnt><aut>Werner Schad</aut>: <vrk>Tra la Andoj boate</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 12a, p. 16a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Schlafsack</trd>
  <trd lng="es">saco de dormir</trd>
  <trdgrp lng="fr">
    <trd>sac de couchage</trd>
  </trdgrp>
  <trd lng="hu">h&aacute;l&oacute;zs&aacute;k</trd>  
</drv>

<drv mrk="sak.dorso0o">
  <kap>dorso<tld/>o</kap>
  <snc>
    <ref tip="dif" cel="tornis.0o">tornistro</ref>
    <ekz>
      <ctl>la dorso<tld/>o de Panjo Rut'</ctl>
      <klr> (novelara titolo)</klr>
      <fnt><aut>Sandor J. Bako, Claude Piron</aut>:
      <vrk>la dorsosako de Panjo Rut'. - Vieno: Pro Esperanto,
      1995</vrk></fnt>
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_n;&c_je;&c_c;</trd>,
    <trd>&c_r;&c_u;&c_k;&c_z;&c_a;&c_k;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_l;&c_je;&c_ch;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">motxila</trd>
  <trd lng="de">Tornister</trd>
  <trdgrp lng="en">
    <trd>pack</trd>,
    <trd>back pack</trd>
  </trdgrp>
  <trd lng="es">mochila</trd>
  <trdgrp lng="fr">
    <trd>sac &agrave; dos</trd>
  </trdgrp>
  <trd lng="hu">h&aacute;tizs&aacute;k</trd>  
  <trd lng="nl">rugzak</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_n;&c_je;&c_c;</trd>,
    <trd>&c_r;&c_ju;&c_k;&c_z;&c_a;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="sak.du0o">
  <kap>du<tld/>o</kap>
  <snc>
    <dif>
      Longa <tld/>o fermita &ccirc;e amba&ubreve; ekstrema&jcirc;oj, kun malfermo en la mezo.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>besace</trd>,
    <trd>bissac</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&aacute;ltalvet&odblac;</trd>,
    <trd>&aacute;talvet&odblac;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_k;&c_i;&c_d;&c_n;&c_a;&c_ja; &c_s;&c_u;&c_m;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="sak.man0o">
  <kap>man<tld/>o</kap>
  <snc>
    <dif>
      Ina <ref tip="super" cel="galant2.0o">galanterio</ref>, malgranda
      <tld/>o mane a&ubreve; &scirc;ultre portebla por kunhavi ekstere diversajn
      utila&jcirc;ojn, kiaj tukoj, okulvitroj, &scirc;minko, skribiloj...
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Handtasche</trd>,
    <trd>Handt&auml;schchen</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>bolsa de mano</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>sac &agrave; main</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&eacute;zit&aacute;ska</trd>,
    <trd>retik&uuml;l</trd>
  </trdgrp>
</drv>
</art>
