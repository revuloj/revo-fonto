  <ofc>*</ofc>
  <rad>sak</rad>/o <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="sak.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sak.0o.KOMUNE">
    <dif>
      Mola <ref tip="super" cel="uj.0o.KOMUNE">ujo</ref> el tolo,
      ledo, papero a&ubreve; cetera materialo kun malferma&jcirc;o &ccirc;e la supro,
      por enteni iajn a&jcirc;ojn, precipe objektojn &scirc;uteblajn (grenon,
      terpomojn, karbojn, sukeron, sablon, po&scirc;ta&jcirc;ojn ktp):
      <ekz>
        <nom>Jozef</nom> ordonis plenigi iliajn <tld/>ojn per greno
        <fnt><bib>MT</bib>, <lok>&Gen; 42:25</lok></fnt>;
      </ekz>
      <ekz>
        jen estas <tld/>o da terpomoj
        <fnt>
          <bib>FK</bib>,
          <lok>La hejmo de la metiisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur siajn azenojn <klr>[ili metis]</klr> malnovajn dis&scirc;iritajn kaj flikitajn
        <ind>vin<tld/>o</ind>jn
        <fnt>
          <bib>MT</bib>
          <lok>Josuo 9:4</lok>
        </fnt>;
        <trd lng="de">Weinschlauch</trd>
      </ekz>
      <ekz>
        anta&ubreve;tukon <klr>[li]</klr> prenis sur la dorson, kiel <ind>pak<tld/>o</ind>n
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
        <trd lng="de">B&uuml;ndel <klr>(auf dem R&uuml;cken)</klr></trd>
      </ekz>
      <ekz>
        &ccirc;iu <tld/>o <klr>(da po&scirc;ta&jcirc;oj)</klr> estas fermita per
        &scirc;nuro, kiun oni povas kuntiri
        <fnt>
          <bib>FK</bib>,
          <lok>El la po&scirc;to</lok>.
        </fnt>;
      </ekz>
      <ekz>
        leterportisto terurita metas la &jcirc;urnalon kaj la leterojn en la <ind>leter<tld/>o</ind>n
        pendantan &ccirc;e lia flanko
        <fnt>
          <bib>IdO</bib>
          <lok>Kvina Parto</lok>
        </fnt>;
        <trd lng="de">Posttasche</trd>
      </ekz>
      <ekz>
        li portis sur si en sia <ind>turist<tld/>o</ind> sian tutan poseda&jcirc;on
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Deka</lok>
        </fnt>;
        <trd lng="de">Tagesrucksack</trd>
      </ekz>
      <ekz>
        <ind>kadavro<tld/>o</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2010/010189.php"
              >Du mil dek</url></vrk>
          <lok>2010</lok>
        </fnt>;
        <trd lng="de">Leichensack</trd>
      </ekz>
      <ekz>
        la libiano malfermis la <ind>voja&gcirc;<tld/>o</ind>n, kiu staris sub la anta&ubreve;a se&gcirc;o
        <fnt>
          <bib>CKv</bib>
          <lok>1</lok>
        </fnt>;
        <trd lng="de">Reisetasche</trd>
      </ekz>
      <ekz>
        li estis ekipita per dorso<tld/>o kaj <tld/>o por kaptiloj
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i prenis <ind>tabako<tld/>o</ind>n el sia po&scirc;o kaj proponis &gcirc;in al <nom>Em&egrave;ric</nom>, tre afable
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 8</lok>
        </fnt>;
        <trd lng="de">Tabakbeutel</trd>
      </ekz>
      <ekz>
        kiel pa&scirc;tistknabo li havis por sia <ind>man&gcirc;o<tld/>o</ind> kvaronon de haringo, kelkajn
        terpomojn, paron da nigraj panpecoj
        <fnt>
          <bib>VRA</bib>
          <lok>Gustav Hedenvind Eriksson</lok>
        </fnt>;
        <trd lng="de">Verpflegungsbeutel</trd>
      </ekz>
      <ekz>
        etaj aparatoj plej ofte estas &jcirc;etitaj en rub<tld/>on kaj atingas la rubfornon
        <fnt>
          <bib>Monato</bib>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="&Monato;/2008/009597.php"
              >Pli kaj pli da rubo al la tria mondo</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni promenadis &ccirc;irka&ubreve;brakumitaj kun <ind>ruba&jcirc;<tld/>o</ind> enmane
        <fnt>
          <bib>OIP</bib>
          <lok>Ombro sur interna pejza&gcirc;o</lok>
        </fnt>;
        <trd lng="de">M&uuml;llbeutel</trd>
      </ekz>
      <ekz>
        se <tld/>o tro pleni&gcirc;as, &gcirc;i balda&ubreve; dis&scirc;iri&gcirc;as
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="posx.0o">po&scirc;o</ref>
    <trd lng="bg">&c_ch;&c_u;&c_v;&c_a;&c_l;</trd>
    <trdgrp lng="de">
      <trd>Sack</trd>,
      <trd>Beutel</trd>
    </trdgrp>
    <trd lng="en">sack</trd>
    <trdgrp lng="es">
      <trd>saco</trd>
    </trdgrp>
    <trd lng="fr">sac</trd>
    <trdgrp lng="hu">
      <trd>t&aacute;ska</trd>,
      <trd>sz&aacute;k</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>worek</trd>,
      <trd>sak</trd>,
      <trd>mieszek<klr> (przest.)</klr></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>saco</trd>
    </trdgrp>
    <trd lng="ru">&c_m;&c_je;&c_sh;&c_o;&c_k;</trd>
  </snc>
  <snc mrk="sak.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="dif" cel="mon.0ujo">Monujo</ref>:
      <ekz>
        fiera pri sia mon<tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam <tld/>o mizeras, amo malaperas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        vortoj <tld/>on ne plenigas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        edzi&gcirc;o pro amo flamanta al <tld/>o sonanta
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kas.0o">kaso</ref>,
      <ref cel="kest.mon0o">monkesto</ref>,
      <ref cel="posx.0o.monujo">po&scirc;o<sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Geldsack</trd>,
      <trd>Geldbeutel</trd>,
      <trd>Geld<ind>b&ouml;rse</ind></trd>,
      <trd>Geldkatze</trd>,
      <trd>Portemonnaie</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>bolsa</trd>,
      <trd>saca</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>bourse<klr> (sac d'argent)</klr></trd>
    </trdgrp>
    <trd lng="hu">p&eacute;nzeszs&aacute;k</trd>
    <trdgrp lng="pl">
      <trd>sakiewka</trd>,
      <trd>mieszek<klr> (przest.)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_sh;&c_je;&c_l;&c_jo;&c_k;</trd>,
      <trd>&c_m;&c_o;&c_sh;&c_n;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sak.0o.tasxo">
    <ref tip="dif" cel="tasx.0o">ta&scirc;o</ref>
    <trd lng="de">Tasche</trd>
    <trdgrp lng="es">
      <trd>ri&ntilde;onera</trd>,
      <trd>faltriquera</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_je;&c_h;</trd>,
    <trd>&c_m;&c_ja;&c_sh;&c_o;&c_k;</trd>,
    <trd>&c_k;&c_a;&c_sh;&c_a;&c_l;&c_jo;&c_k;</trd>
  </trdgrp>
  <trd lng="bo">&t_la;&t_tata;&t_naro;&t_tsheg;&t_pha;&t_da;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>pouzdro</trd>,
    <trd>pytel</trd>,
    <trd>vak</trd>,
    <trd>v&aacute;&ccaron;ek</trd>,
    <trd>v&yacute;chlipka</trd>
  </trdgrp>
  <trd lng="en">bag</trd>
  <trdgrp lng="es">
    <trd>bolsa</trd>,
    <trd>ri&ntilde;onera</trd>
  </trdgrp>
  <trd lng="id">tas</trd>
  <trd lng="it">sacco</trd>
  <trdgrp lng="ja">
    <trd>&#34955;  <pr>&#12383;&#12356;</pr></trd>,
    <trd>&#12363;&#12400;&#12435;</trd>,
    <trd>&#12496;&#12483;&#12464;  <pr>&#12400;&#12387;&#12368;</pr></trd>
  </trdgrp>
  <trd lng="nl">zak</trd>
  <trdgrp lng="sk">
    <trd>puzdro</trd>,
    <trd>vak</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_m;&c_ib;&c_sh;&c_o;&c_k;</trd>,
    <trd>&c_s;&c_u;&c_m;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_o;&c_r;&c_b;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="sak.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="sak.0ajxo.BIB">
    <uzo tip="fak">BIB</uzo>
    <dif>
      Maldelikata pentovesto (el kruda kaprohara &scirc;tofo, kian oni
      antikve uzis por fari <tld/>ojn<sncref ref="sak.0o.KOMUNE"/>):
      <ekz>
        vi deprenis de mi mian <tld/>a&jcirc;on kaj zonis min per
        &gcirc;ojo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">saco<klr> (del penitente)</klr></trd>
  <trdgrp lng="fr">
    <trd>sac<klr> (bibl. v&ecirc;tement de p&eacute;nitence)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>sacco<klr> (abito del penitente)</klr></trd>
  </trdgrp>
  <trd lng="nl">boetekleed</trd>
  <trdgrp lng="pl">
    <trd>worek pokutny</trd>,
    <trd>w&lstroke;osienica</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_l;&c_a;&c_s;&c_ja;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="uk">&c_m;&c_ib;&c_sh;&c_k;&c_o;&c_v;&c_i;&c_n;&c_a;</trd>
</drv>

<drv mrk="sak.acxet0o">
  <kap>
    a&ccirc;et<tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="S"/>o por porti a&ccirc;eta&jcirc;ojn:
      <ekz>
        eliris malgrasa virineto kun ladbotelo kaj a&ccirc;et<tld/>o en la manoj
        <fnt>
          <bib>MkM</bib>
          <lok>24</lok>
        </fnt>;
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Einkaufstasche</trd>
</drv>

<drv mrk="sak.almozo0o">
  <kap>
    almozo<tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="S"/>o en kiu oni kolektas mondonacojn:
      <ekz>
        mi &ccirc;iam esperis, ke anka&ubreve; la malri&ccirc;uloj venos al la meso, enrigardante la
        almozo<tld/>on mi rimarkas, ke nur ili venadas
        <fnt>
          <bib>Monato</bib>
          <aut>S. Maul</aut>
          <vrk>Du patroj interparolas.</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Kollektebeutel</trd>
</drv>

<drv mrk="sak.blov0o">
  <kap>
    blov<tld/>o, <var><kap>vent<tld/>o</kap></var>
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="blov.0ilo">Blovilo</ref> (de for&gcirc;isto, sakfluto):
      <ekz>
        ardon <klr>[&#8230;]</klr> trovinte, li komencis disblovi la fajron <klr>[&#8230;]</klr> per
        for&gcirc;ista blov<tld/>o
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi traktus la duelinviton de tiuj vent<tld/>oj
        <klr>(kapriculoj, senta&ubreve;guloj)</klr> kun malestimo
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Blasebalg</trd>
</drv>

<drv mrk="sak.dormo0o">
  <kap>dormo<tld/>o</kap>
  <snc mrk="sak.dormo0o.TUR">
    <dif>
      Remburita kovrilo por endormo, kun unu nefermita ekstremo,
      uzata kiel porvoja&gcirc;a lita&jcirc;o:
      <ekz>
        ni ja kunportis dormo<tld/>ojn
        <fnt>
          <aut>W. Schad</aut>,
          <vrk>Tra la Andoj boate</vrk>,
          <bib>Monato</bib>,
          <lok>2001:12, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_s;&c_p;&c_a;&c_l;&c_je;&c_n; &c_ch;&c_u;&c_v;&c_a;&c_l;</trd>
  <trd lng="cs">spac&iacute; pytel</trd>
  <trd lng="de">Schlafsack</trd>
  <trd lng="es">saco de dormir</trd>
  <trd lng="fr">sac de couchage</trd>
  <trd lng="hu">h&aacute;l&oacute;zs&aacute;k</trd>
  <trd lng="id"><ind>kantung</ind> tidur</trd>
  <trd lng="it"><ind>sacco</ind> a pelo</trd>
  <trd lng="ja">&#23517;&#34955;  <pr>&#12397;&#12406;&#12367;&#12429;</pr></trd>
  <trd lng="nl">slaapzak</trd>
  <trd lng="pl">&sacute;piw&oacute;r</trd>
  <trd lng="ru">&c_s;&c_p;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_m;&c_je;&c_sh;&c_o;&c_k;</trd>
  <trd lng="sk">spac&iacute; vak</trd>
  <trd lng="uk">&c_s;&c_p;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j; &c_m;&c_ib;&c_sh;&c_o;&c_k;</trd>
</drv>

<drv mrk="sak.dorso0o">
