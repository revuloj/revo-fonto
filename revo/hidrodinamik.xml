  <trd lng="nl">hydrodynamica</trd>
  <trd lng="pl">hydrodynamika</trd>
  <trd lng="uk">&#x433;&#x456;&#x434;&#x440;&#x43E;&#x434;&#x438;&#x43D;&#x430;&#x43C;&#x456;&#x43A;&#x430;</trd>
</drv>
