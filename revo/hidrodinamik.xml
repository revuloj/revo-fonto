  <trd lng="ca">hidrodin&agrave;mica</trd>
  <trd lng="fr">hydrodynamique</trd>
  <trd lng="ja">&#27969;&#20307;&#21147;&#23398; 
  <pr>&#12426;&#12421;&#12358;&#12383;&#12356;&#12426;&#12365;&#12364;&#12367;</pr></trd>
  <trd lng="nl">hydrodynamica</trd>
  <trd lng="pl">hydrodynamika</trd>
  <trd lng="uk">&c_g;&c_ib;&c_d;&c_r;&#1086;&c_d;&c_i;&#1085;&c_a;&#1084;&c_ib;&#1082;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#27969;&#20307;&#21147;&#23398; <pr>li&#250;t&#464;l&#236;xu&#233;</pr></trd>,
    <trd>&#27700;&#21160;&#21147;&#23398; <pr>shu&#464;d&#242;ngl&#236;xu&#233;</pr></trd>
