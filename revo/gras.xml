<drv mrk="gras.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="gras.mal0a.sengrasa">
    <dif>
      Havanta malmulte da <tld/>o:
      <ekz>
        bovinoj malbelaspektaj kaj mal<tld/>aj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 41:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;afo mal<tld/>a
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 34:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        longaj, mal<tld/>aj fingroj
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>a asketo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        Sara estis malsana: mal<tld/>a, pala, &scirc;iaj grandaj okuloj enfalis
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la mal<tld/>aj &ccirc;evaloj kuris tiel, ke el ili iris vaporo
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de la jaro</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_h;&c_u;&c_d;&c_y;</trd>,
      <trd>&c_s;&c_h;&c_u;&c_d;&c_n;&c_je;&c_l;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>magro<klr tip="ind"> (adj.)</klr></trd>,
      <trd>flaco</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>sov&aacute;ny</trd>,
      <trd>szik&aacute;r</trd>
    </trdgrp>
    <trd lng="nl">mager</trd>
    <trdgrp lng="pl">
      <trd>chudy</trd>,
      <trd>lichy</trd>
    </trdgrp>
    <trd lng="pt">magro<klr tip="ind"> (adj.)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_o;&c_shch;&c_i;&c_j;</trd>,
      <trd>&c_h;&c_u;&c_d;&c_o;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="gras.mal0a.nesterkita">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Malmulte fruktodona, malbone sterkita:
      <ekz>
        mal<tld/>a grundo;
      </ekz>
      <ekz>
        kelke da mal<tld/>aj grenkampoj
        <fnt>
          <bib>Fab3</bib>
          <lok>Ib kaj malgranda kristino</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;evaloj kaj bovoj promenadis kune sur la mal<tld/>a pa&scirc;tejo
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ricx.mal0a">malri&ccirc;a</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_n;&c_je;&c_w;&c_r;&c_a;&c_d;&c_l;&c_ib;&c_v;&c_y;</trd>,
      <trd>&c_b;&c_ja;&c_s;&c_h;&c_l;&c_je;&c_b;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="es">est&eacute;ril<klr tip="amb"> (adj., tierra)</klr></trd>
    <trd lng="hu">sov&aacute;ny</trd>
    <trd lng="pl">lichy</trd>
    <trd lng="pt">pobre<klr tip="amb"> (adj., terra)</klr></trd>
  </snc>
  <trd lng="cs">huben&yacute;</trd>
  <trdgrp lng="de">
    <trd>mager</trd>,
    <trd>d&uuml;nn</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>maigre</trd>,
    <trd>pauvre<klr tip="ind"> (en p. d'un sol)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12420;&#x305B;&#x305F;</trd>,
    <trd>&#x9AA8;&#x5F35;&#12387;&#x305F;  <pr>&#12371;&#12388;&#x306F;&#12387;&#x305F;</pr></trd>,
    <trd>&#33026;&#x6C17;&#x306E;&#x306A;&#12356;  <pr>&#12354;&#12406;&#12425;&#x304D;&#x306E;&#x306A;&#12356;</pr></trd>,
    <trd>&#33026;&#x80AA;&#20998;&#x306E;&#x5C11;&#x306A;&#12356;  <pr>&#12375;&#x307C;&#12358;&#12406;&#12435;&#x306E;&#12377;&#x304F;&#x306A;&#12356;</pr></trd>,
    <trd>&#x4E0D;&#x6BDB;&#x306E;  <pr>&#12405;&#12418;&#12358;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">arm<klr tip="ind"> (mager)</klr></trd>
  <trd lng="sk">chud&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_ib;&c_s;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_h;&c_u;&c_d;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x75E9;&#38263;&#30340; <pr>sh&#xF2;uch&#xE1;ngde</pr></trd>,
    <trd>&#30246;&#38263;&#30340; <pr>sh&#xF2;uch&#xE1;ngde</pr></trd>,
    <trd>&#30246; <pr>sh&#xF2;u</pr></trd>,
    <trd>&#x7FB8; <pr>l&#xE9;i</pr></trd>,
    <trd>&#30240; <pr>j&#xED;</pr></trd>,
    <trd>&#x6DF8;&#x766F; <pr>q&#x12B;ngq&#xFA;</pr></trd>,
    <trd>&#x524A;&#30246; <pr>xi&amacron;osh&#xF2;u</pr></trd>,
    <trd>&#x76AE;&#21253;&#x9AA8;&#22836; <pr>p&#xED;b&amacron;og&#x1D4;t&#xF3;u</pr></trd>,
    <trd>&#x9AA8;&#30246;&#22914;&#x67F4; <pr>g&#x1D4;sh&#xF2;ur&#xFA;ch&#xE1;i</pr></trd>,
    <trd>&#x4F4E;&#33026; <pr>d&#x12B;zh&#x12B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="gras.mal0igxi">
  <kap>mal<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="gras.mal0igxi.DIET">
    <dif>
      Seni&gcirc;i je <tld/>o,
      <ref tip="vid" cel="dik.mal0igxi">maldiki&gcirc;i</ref>:
      <ekz>
        vi tiel mal<tld/>i&gcirc;as
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 13:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        li povis vidi, ke li multe mal<tld/>i&gcirc;is
        <fnt>
          <aut>Lao She, trad. Wang Chongfang</aut>,
          <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0026/">
          Kamelo &Scirc;jangzi</url></vrk>,
          <lok>1988</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">abnehmen</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_u;&c_d;&c_n;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_ib;&c_z;&c_a;&c_r;&c_n;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="es">adelgazar</trd>
  <trd lng="fr">maigrir</trd>
  <trd lng="ja">&#12420;&#x305B;&#x308B;</trd>
  <trd lng="uk">&c_h;&c_u;&c_d;&c_n;&c_u;&c_t;&c_i;</trd>
</drv>
