    <trd>&#33026;&#32938;&#20998;&#12398;&#23569;&#12394;&#12356;  <pr>&#12375;&#12412;&#12358;&#12406;&#12435;&#12398;&#12377;&#12367;&#12394;&#12356;</pr></trd>,
    <trd>&#19981;&#27611;&#12398;  <pr>&#12405;&#12418;&#12358;&#12398;</pr></trd>
  </trdgrp>
  <trd lng="nl">arm<klr tip="ind"> (mager)</klr></trd>
  <trd lng="sk">chud&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_ib;&c_s;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_h;&c_u;&c_d;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#30185;&#38263;&#30340; <pr>sh&ograve;uch&aacute;ngde</pr></trd>,
