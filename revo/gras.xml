<drv mrk="gras.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="gras.mal0a.sengrasa">
    <dif>
      Havanta malmulte da <tld/>o:
      <ekz>
        bovinoj malbelaspektaj kaj mal<tld/>aj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 41:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;afo mal<tld/>a
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 34:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        longaj, mal<tld/>aj fingroj
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>a asketo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        Sara estis malsana: mal<tld/>a, pala, &scirc;iaj grandaj okuloj enfalis
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la mal<tld/>aj &ccirc;evaloj kuris tiel, ke el ili iris vaporo
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de la jaro</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_h;&c_u;&c_d;&c_y;</trd>,
      <trd>&c_s;&c_h;&c_u;&c_d;&c_n;&c_je;&c_l;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>magro<klr tip="ind"> (adj.)</klr></trd>,
      <trd>flaco</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>sov&aacute;ny</trd>,
      <trd>szik&aacute;r</trd>
    </trdgrp>
    <trd lng="nl">mager</trd>
    <trdgrp lng="pl">
      <trd>chudy</trd>,
      <trd>lichy</trd>
    </trdgrp>
    <trd lng="pt">magro<klr tip="ind"> (adj.)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_o;&c_shch;&c_i;&c_j;</trd>,
      <trd>&c_h;&c_u;&c_d;&c_o;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="gras.mal0a.nesterkita">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Malmulte fruktodona, malbone sterkita:
      <ekz>
        mal<tld/>a grundo;
      </ekz>
      <ekz>
        kelke da mal<tld/>aj grenkampoj
        <fnt>
          <bib>Fab3</bib>
          <lok>Ib kaj malgranda kristino</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;evaloj kaj bovoj promenadis kune sur la mal<tld/>a pa&scirc;tejo
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ricx.mal0a">malri&ccirc;a</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_n;&c_je;&c_w;&c_r;&c_a;&c_d;&c_l;&c_ib;&c_v;&c_y;</trd>,
      <trd>&c_b;&c_ja;&c_s;&c_h;&c_l;&c_je;&c_b;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="es">est&eacute;ril<klr tip="amb"> (adj., tierra)</klr></trd>
    <trd lng="hu">sov&aacute;ny</trd>
    <trd lng="pl">lichy</trd>
    <trd lng="pt">pobre<klr tip="amb"> (adj., terra)</klr></trd>
  </snc>
  <trd lng="cs">huben&yacute;</trd>
  <trdgrp lng="de">
    <trd>mager</trd>,
    <trd>d&uuml;nn</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>maigre</trd>,
    <trd>pauvre<klr tip="ind"> (en p. d'un sol)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
