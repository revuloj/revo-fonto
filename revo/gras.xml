<drv mrk="gras.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc mrk="gras.0ajxo.io">
    <dif>
      Io <tld/>a:
      <ekz>
        man&gcirc;u <tld/>a&jcirc;on kaj trinku dol&ccirc;a&jcirc;on
        <fnt>
          <bib>MT</bib>
          <lok>Ne&hcirc;emja 8:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi havus pacon &ccirc;e via tablo, plena de <tld/>a&jcirc;oj
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 36:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i &scirc;miru mian pordon per lupa <tld/>a&jcirc;o kaj eksidu &ccirc;e mia hejmfajro
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXXIV</lok>
        </fnt>!
      </ekz>
      <ekz>
        el la <tld/>a&jcirc;o fari&gcirc;as sapo
        <fnt>
          <bib>VaK</bib>
          <lok>Sepa &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni ricevis nur tricent gramojn da pano kaj apena&ubreve; sep gramojn da <tld/>a&jcirc;o
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 11 (61)</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Fettigkeit</trd>,
      <trd>Schlemmerei</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#28024;&#27833; <pr>j&igrave;ny&oacute;u</pr></trd>,
    <trd>&#27833;&#36074; <pr>y&oacute;uzh&igrave;</pr></trd>,
    <trd>&#39135;&#36947;&#20048; <pr>sh&iacute;d&agrave;ol&egrave;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_t;&c_l;&c_u;&c_s;&c_t;&c_a;&c_je; <klr>(&c_n;&c_a;&c_z;.)</klr></trd>
  <trd lng="ja">&#33026;&#36523;  <pr>&#12354;&#12406;&#12425;&#12415;</pr></trd>
  <trdgrp lng="uk">
    <trd>&c_zh;&c_i;&c_r;&c_n;&c_je;</trd>,
    <trd>&c_s;&c_k;&c_o;&c_r;&c_o;&c_m;&c_n;&c_je; <klr>(&c_u; &c_z;&c_n;&c_a;&c_ch;. &c_ib;&c_m;.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="gras.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="gras.0igi.dikigi">
    <dif>
      Fari iun <tld/>a:
      <ekz>
        <tld/>igi porkon;
      </ekz>
      <ekz>
        la <tld/>igitan &scirc;afon vi bu&ccirc;as, sed la &scirc;afojn vi ne pa&scirc;tas
        <fnt>
