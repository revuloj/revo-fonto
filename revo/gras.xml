    <trd>&c_s;&c_k;&c_o;&c_r;&c_o;&c_m;&c_n;&c_je; <klr>(&c_u; &c_z;&c_n;&c_a;&c_ch;. &c_ib;&c_m;.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="gras.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="gras.0igi.dikigi">
    <dif>
      Fari iun <tld/>a:
      <ekz>
        <tld/>igi porkon;
      </ekz>
      <ekz>
        la <tld/>igitan &scirc;afon vi bu&ccirc;as, sed la &scirc;afojn vi ne pa&scirc;tas
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 34:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        per la oferoj sin <tld/>igas la pastroj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        la anseron <klr>[oni]</klr> <tld/>igis dum ok semajnoj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Feli&ccirc;a Jo&ccirc;jo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi man&gcirc;as aeron, oni min <tld/>igas per promesoj
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_u;&c_k;&c_o;&c_r;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_d;&c_k;&c_o;&c_r;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_g;&c_l;&c_a;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">m&auml;sten</trd>
    <trd lng="es">engordar</trd>
    <trd lng="hu">h&iacute;zlal</trd>
    <trd lng="nl">vetmesten</trd>
    <trd lng="pl">tuczy&cacute;</trd>
    <trd lng="pt">engordar</trd>
    <trd lng="ru">&c_o;&c_t;&c_k;&c_o;&c_r;&c_m;&c_i;&c_t;&c_mol;</trd>
    <trd lng="zh">&#x6B7B;&#x8A18; <pr>s&#x1D0;j&#xEC;</pr></trd>
</snc>
  <snc mrk="gras.0igi.sxmiri">
    <dif>
      Fari ion <tld/>a, &scirc;miri per <tld/>o:
      <ekz>
        <tld/>igi paton, &scirc;uojn, pafiltubon;
      </ekz>
      <ekz>
        ne <tld/>igu per mensogo viajn nova&jcirc;ojn
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_t;&c_l;&c_u;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;</trd>,
      <trd>&c_sh;&c_m;&c_a;&c_l;&c_mol;&c_c;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_m;&c_a;&c_z;&c_a;&c_c;&c_mol; &c_t;&c_l;&c_u;&c_ch;&c_sh;&c_a;&c_m;</trd>
    </trdgrp>
    <trd lng="de">fetten</trd>
    <trdgrp lng="es">
      <trd>untar</trd>,
      <trd>engrasar</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>bezs&iacute;roz</trd>,
      <trd>zs&iacute;rral beken</trd>
    </trdgrp>
    <trd lng="nl">invetten</trd>
    <trdgrp lng="pl">
      <trd>nat&lstroke;uszcza&cacute;</trd>,
      <trd>okrasza&cacute;<klr> (dot. pokarm&oacute;w)</klr></trd>
    </trdgrp>
    <trd lng="pt">untar</trd>
    <trd lng="ru">
    &c_s;&c_m;&c_a;&c_z;&c_a;&c_t;&c_mol; &c_zh;&c_i;&c_r;&c_o;&c_m;
    </trd>
  </snc>
  <snc mrk="gras.0igi.sterki">
    <uzo tip="fak">AGR</uzo>
    <dif>
      <ref tip="dif" cel="sterk.0i">Sterki</ref>:
      <ekz>
        <tld/>igi teron, &gcirc;ardenon.
      </ekz>
    </dif>
    <trd lng="be">&c_u;&c_g;&c_n;&c_o;&c_j;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="de">d&uuml;ngen</trd>
    <trd lng="es">abonar</trd>
    <trd lng="hu">tr&aacute;gy&aacute;z</trd>
    <trd lng="nl">bemesten</trd>
    <trd lng="pt">estercar</trd>
    <trd lng="ru">&c_u;&c_d;&c_o;&c_b;&c_r;&c_ja;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>hnojit</trd>,
    <trd>krmit</trd>,
    <trd>vykrmit</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>graisser</trd>,
    <trd>engraisser<klr tip="ind"> (tr.)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x80A5;&#12360;&#12373;&#x305B;&#x308B;  <pr>&#12371;&#12360;&#12373;&#x305B;&#x308B;</pr></trd>
  <trdgrp lng="sk">
    <trd>masti&tcaron;</trd>,
    <trd>vyk&racute;mi&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_g;&c_o;&c_d;&c_o;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_u;&c_d;&c_o;&c_b;&c_r;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="gras.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="gras.0igxi.KOMUNE">
    <dif>
      I&gcirc;i <tld/>a:
      <ekz>
        la porkoj tre facile <tld/>i&gcirc;as;
      </ekz>
      <ekz>
        ili man&gcirc;os kaj sati&gcirc;os kaj <tld/>i&gcirc;os
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 31:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la akciuloj <tld/>i&gcirc;as <klr>(ri&ccirc;i&gcirc;as)</klr>
        per altaj dividendoj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_l;&c_u;&c_s;&c_mol;&c_c;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_l;&c_a;&c_d;&c_ch;&c_e;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_y;&c_c;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>engordar<klr tip="amb"> (hacerse gordo)</klr></trd>,
    <trd>cebarse<klr tip="ind"> (intr.)</klr></trd>
