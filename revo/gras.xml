  <trdgrp lng="fr">
    <trd>grossir</trd>,
    <trd>engraisser<klr tip="ind"> (intr.)</klr></trd>
  </trdgrp>
  <trd lng="hu">h&iacute;zik</trd>
  <trd lng="ja">&#x80A5;&#12360;&#x308B;  <pr>&#12371;&#12360;&#x308B;</pr></trd>
  <trd lng="nl">vet worden</trd>
  <trdgrp lng="pl">
