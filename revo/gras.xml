    <trd>&#30240; <pr>j&iacute;</pr></trd>,
    <trd>&#28152;&#30319; <pr>q&imacron;ngq&uacute;</pr></trd>,
    <trd>&#21066;&#30246; <pr>xi&amacron;osh&ograve;u</pr></trd>,
    <trd>&#30382;&#21253;&#39592;&#22836; <pr>p&iacute;b&amacron;og&#468;t&oacute;u</pr></trd>,
    <trd>&#39592;&#30246;&#22914;&#26612; <pr>g&#468;sh&ograve;ur&uacute;ch&aacute;i</pr></trd>,
    <trd>&#20302;&#33026; <pr>d&imacron;zh&imacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="gras.mal0igxi">
