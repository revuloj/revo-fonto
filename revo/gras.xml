<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>gras</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="gras.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gras.0o.besta">
    <fnt>Z</fnt>
    <dif>
      &Scirc;mirta&ubreve;ga kaj facile fandebla substanco en la korpo
      de bestoj.
    </dif>
  </snc>
  <snc mrk="gras.0o.nebesta">
    <dif>
      Similnatura substanco el diversa deveno:
      <ekz>
        oleo estas vegeta&jcirc;a, vazelino minerala <tld/>o.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="sub">
    <ref cel="lard.0o">lardo</ref>,
    <ref cel="seb.0o">sebo</ref>,
    <ref cel="buter.0o">butero</ref>,
    <ref cel="margar.0o">margarino</ref>,
    <ref cel="vegeta1.0o">vegetalino</ref>.
  </refgrp>
  <trd lng="de">Fett</trd>
  <trd lng="en">grease</trd>
  <trd lng="es">grasa</trd>
  <trd lng="fr">graisse</trd>
  <trd lng="hu">zs&iacute;r</trd>  
  <trd lng="nl">vet</trd>
  <trd lng="pl">t&lstroke;uszcz</trd>
  <trd lng="pt">gordura</trd>
  <trd lng="ru">&c_zh;&c_i;&c_r;</trd>
  <trd lng="tr">ya&gbreve;</trd>
  <trdgrp lng="sk">
    <trd>loj</trd>,
    <trd>olej</trd>,
    <trd>sadlo</trd>,
    <trd>tuk</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>omastek</trd>,
    <trd>rostlinný tuk</trd>,
    <trd>sádlo</trd>
  </trdgrp>
</drv>

<drv mrk="gras.0a">
  <kap><tld/>a</kap>
  <snc mrk="gras.0a.elGraso">
    <dif>
      Konsistanta el <tld/>o, natursimila al <tld/>o:
      <ekz>
        <tld/>a parto de viando;
      </ekz>
      <ekz>
        <ind><tld/>a makulo</ind>;
        <trd lng="hu">zs&iacute;rfolt</trd>
        <trd lng="nl">vetvlek</trd>
      </ekz>
      <ekz>
        <tld/>a &scirc;mira&jcirc;o.
      </ekz>
    </dif>
    <trd lng="de">fettig</trd>
    <trd lng="es">graso</trd>
    <trdgrp lng="hu">
      <trd>zs&iacute;ros</trd>,
      <trd>zs&iacute;r-</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>vet</trd>,
      <trd>vettig</trd>
    </trdgrp>
    <trd lng="pl">t&lstroke;usty</trd>
    <trd lng="pt">gorduroso</trd>
    <trdgrp lng="ru">
      <trd>&c_zh;&c_i;&c_r;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_zh;&c_i;&c_r;&c_o;&c_v;&c_o;&c_j;</trd>,
      <trd>&c_s;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="gras.0a.dika">
    <dif>
      Havanta multe da <tld/>o, dika:
      <ekz>
        <tld/>a viando, ansero;
      </ekz>
      <ekz>
        homo tre <tld/>dika
        <fnt><bib>MT</bib>, <lok>&Jug; 3:18</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">fett</trd>
    <trd lng="es">grasoso</trd>
    <trdgrp lng="hu">
      <trd>k&ouml;v&eacute;r</trd>,
      <trd>h&aacute;jas</trd>
    </trdgrp>
    <trd lng="nl">vet</trd>
    <trd lng="pl">t&lstroke;usty</trd>
    <trd lng="pt">gordo</trd>
    <trd lng="ru">&c_zh;&c_i;&c_r;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="gras.0a.fruktodona">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Bone sterkita, fruktodona:
      <ekz>
        <tld/>a grundo, kampo.
      </ekz>
    </dif>
    <trd lng="pl">t&lstroke;usty</trd>
    <trd lng="pt">rica<klr tip="amb"> (adj., terra)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_zh;&c_i;&c_r;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_t;&c_u;&c_ch;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="gras.0a.tip">
    <uzo tip="fak">TIP</uzo>
    <fnt><bib>KompLeks</bib></fnt>
    <dif>
      Tipara fasono pli peza ol tiu uzata por la &ccirc;efteksto.
    </dif>
    <rim>
      Iuj tiparaj familioj disponigas plurajn tiajn <tld/>ajn fasonojn;
      tiu uzata en kompostado de libroj kutime estas relative malpli
      peza (la plej pezajn oni uzas por specialaj efektoj en
      afi&scirc;oj kaj similaj arta&jcirc;oj). Tial oni povus distingi
      tiajn pezogradojn uzante la vortojn <tld/>eta, <tld/>ega.
      &Scirc;ajnas ke PIV1 elektis la vorton <tld/>eta por nomi la plej
      ordinaran specon (simile al la rusa tradicio); ni tamen opinias
      pli oportuna uzi la radikvorton por la ordinara speco, kaj
      la&ubreve;bezone precizigi per sufikso (ekz-e, <tld/>ega).
    <fnt><bib>KompLeks</bib></fnt>
    </rim>
    <ref tip="vid" cel="kursiv.0o">kursivo</ref>
    <trdgrp lng="en">
      <trd>boldface</trd>,
      <trd>bold</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>wyt&lstroke;uszczony<klr> (o czcionce, pi&sacute;mie)</klr>
      </trd>,
      <trd>pogrubiony<klr> (o czcionce, pi&sacute;mie)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_zh;&c_i;&c_r;&c_n;&c_y;&c_j;
    <klr> &c_sh;&c_r;&c_i;&c_f;&c_t;</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>tlustý</trd>,
    <trd>tučný</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gras</trd>,
    <trd>riche<klr tip="ind"> (en p. d'un sol)</klr></trd>
  </trdgrp>
  <trd lng="hu">zs&iacute;ros</trd>
  <trd lng="nl">vet</trd>
  <trdgrp lng="sk">
    <trd>tlstý</trd>,
    <trd>tučný</trd>
  </trdgrp>
  <trd lng="tr">ya&gbreve;l&inodot;</trd>
</drv>

<drv mrk="gras.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="gras.0igi.dikigi">
    <dif>
      Fari iun <tld/>a:
      <ekz>
        <tld/>igi porkon.
      </ekz>
    </dif>
    <trd lng="de">m&auml;sten</trd>
    <trd lng="es">engordar</trd>
    <trd lng="hu">h&iacute;zlal</trd>    
    <trd lng="nl">vetmesten</trd>
    <trd lng="pl">tuczy&cacute;</trd>
    <trd lng="pt">engordar</trd>
    <trd lng="ru">&c_o;&c_t;&c_k;&c_o;&c_r;&c_m;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="gras.0igi.sxmiri">
    <dif>
      Fari ion <tld/>a, &scirc;miri per <tld/>o:
      <ekz>
        <tld/>igi paton, &scirc;uojn, pafiltubon.
      </ekz>
    </dif>
    <trd lng="de">fetten</trd>
    <trdgrp lng="es">
      <trd>untar</trd>,
      <trd>engrasar</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>bezs&iacute;roz</trd>,
      <trd>zs&iacute;rral beken</trd>
    </trdgrp>
    <trd lng="nl">invetten</trd>
    <trdgrp lng="pl">
      <trd>nat&lstroke;uszcza&cacute;</trd>,
      <trd>okrasza&cacute;<klr> (dot. pokarm&oacute;w)</klr></trd>
    </trdgrp>
    <trd lng="pt">untar</trd>
    <trd lng="ru">
    &c_s;&c_m;&c_a;&c_z;&c_a;&c_t;&c_mol; &c_zh;&c_i;&c_r;&c_o;&c_m;
    </trd>
  </snc>
  <snc mrk="gras.0igi.sterki">
    <uzo tip="fak">AGR</uzo>
    <ref tip="dif" cel="sterk.0i">sterki</ref>
    <ekz>
      <tld/>igi teron, &gcirc;ardenon.
    </ekz>
    <trd lng="de">d&uuml;ngen</trd>
    <trd lng="es">abonar</trd>
    <trd lng="hu">tr&aacute;gy&aacute;z</trd>    
    <trd lng="nl">bemesten</trd>
    <trd lng="pt">estercar</trd>
    <trd lng="ru">&c_u;&c_d;&c_o;&c_b;&c_r;&c_ja;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>hnojit</trd>,
    <trd>krmit</trd>,
    <trd>vykrmit</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>graisser</trd>,
    <trd>engraisser<klr tip="ind"> (tr.)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mastiť</trd>,
    <trd>vykŕmiť</trd>
  </trdgrp>
</drv>

<drv mrk="gras.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      I&gcirc;i <tld/>a:
      <ekz>
        la porkoj tre facile <tld/>i&gcirc;as;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la akciuloj <tld/>i&gcirc;as <klr>(ri&ccirc;i&gcirc;as)</klr>
        per altaj dividendoj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd>engordar<klr tip="amb"> (hacerse gordo)</klr></trd>,
    <trd>cebarse<klr tip="ind"> (intr.)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>grossir</trd>,
    <trd>engraisser<klr tip="ind"> (intr.)</klr></trd>
  </trdgrp>
  <trd lng="hu">h&iacute;zik</trd>  
  <trd lng="nl">vet worden</trd>
  <trdgrp lng="pl">
    <trd>ty&cacute;</trd>,
    <trd>uty&cacute;</trd>,
    <trd>rozty&cacute; si&eogonek;</trd>,
    <trd>tuczy&cacute; si&eogonek;</trd>,
    <trd>utuczy&cacute; si&eogonek;</trd>
  </trdgrp>
  <trd lng="pt">engordar<klr tip="amb"> (tornar-se gordo)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_i;&c_r;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_t;&c_u;&c_ch;&c_n;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="tr">&scedil;i&scedil;manlamak</trd>
</drv>

<drv mrk="gras.0igajxo">
  <kap><tld/>iga&jcirc;o</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <ref tip="dif" cel="sterk.0o">sterko</ref>
  </snc>
  <trdgrp lng="es">
   <trd>estiercol</trd>, 
   <trd>abono</trd>,
   <trd>mantillo</trd> 
  </trdgrp>
  <trd lng="fr">engrais</trd>
  <trd lng="hu">tr&aacute;gya</trd>  
  <trd lng="nl">mest</trd>
  <trdgrp lng="pt">
   <trd>esterco</trd>, 
   <trd>estrume</trd> 
  </trdgrp>
  <trd lng="ru">&c_u;&c_d;&c_o;&c_b;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="gras.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="gras.mal0a.sengrasa">
    <dif>
      Havanta malmulte da <tld/>o:
      <ekz>
        la sep malgrasaj bovinoj el la Biblio.
      </ekz>
    </dif>
    <trdgrp lng="es">
      <trd>magro<klr tip="ind"> (adj.)</klr></trd>,
      <trd>flaco</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>sov&aacute;ny</trd>,
      <trd>szik&aacute;r</trd>
    </trdgrp>
    <trd lng="nl">mager</trd>
    <trdgrp lng="pl">
      <trd>chudy</trd>,
      <trd>lichy</trd>
    </trdgrp>
    <trd lng="pt">magro<klr tip="ind"> (adj.)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_o;&c_shch;&c_i;&c_j;</trd>,
      <trd>&c_h;&c_u;&c_d;&c_o;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="gras.mal0a.nesterkita">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Malmulte fruktodona, malbone sterkita:
      <ekz>
        mal<tld/>a grundo
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ricx.mal0a">malri&ccirc;a</ref>.
    </refgrp>
    <trd lng="es">est&eacute;ril<klr tip="amb"> (adj., tierra)</klr></trd>
    <trd lng="hu">sov&aacute;ny</trd>
    <trd lng="pl">lichy</trd>
    <trd lng="pt">pobre<klr tip="amb"> (adj., terra)</klr></trd>
  </snc>
  <trd lng="cs">hubený</trd>
  <trd lng="de">mager</trd>
  <trdgrp lng="fr">
    <trd>maigre</trd>,
    <trd>pauvre<klr tip="ind"> (en p. d'un sol)</klr></trd>
  </trdgrp>
  <trd lng="nl">arm<klr tip="ind"> (mager)</klr></trd>
  <trd lng="sk">chudý</trd>
</drv>

<drv mrk="gras.mal0igxi">
  <kap>mal<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="gras.mal0igxi.DIET">
    <dif>
      Seni&gcirc;i je <tld/>o,
      <ref tip="vid" cel="dik.mal0igxi">maldiki&gcirc;i</ref>:
      <ekz>
        li povis vidi, ke li multe mal<tld/>i&gcirc;is
        <fnt>
          <aut>Lao She, trad. Wang Chongfang</aut>,
          <vrk><url
          ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0026/">
          Kamelo &Scirc;jangzi</url></vrk>,
          <lok>1988</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">adelgazar</trd>
  <trd lng="fr">maigrir</trd>
</drv>
</art>
