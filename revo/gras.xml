<drv mrk="gras.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gras.0o.besta">
    <dif>
      Energiri&ccirc;a substanco en la &ccirc;eloj de bestoj kaj plantoj, &hcirc;emie acilglicerinoj
      (pli tradicia nomo <ref tip="sin" cel="glicer1.0o">glicerido</ref>j);
      tiujn, kiuj estas fluaj je 20&deg;C oni nomas anka&ubreve; <ref tip="sub" cel="ole.0o">oleo</ref>j:
      <ekz>
        oleo estas vegeta&jcirc;a, vazelino minerala <tld/>o;
      </ekz>
      <ekz>
        konsumi sian anta&ubreve;an <tld/>on
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        miaj genuoj senforti&gcirc;is de fastado, kaj mia karno perdis la <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 109:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> nutris lin per <klr>[&#8230;]</klr> butero de bovinoj kaj per
        <klr>[&#8230;]</klr> sebo de &scirc;afidoj <klr>[&#8230;]</klr> kaj per la <tld/>o de la kernoj
        de tritiko
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 32:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>hepata&jcirc;o, anasoj a&ubreve; anseroj konservitaj en <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Marc Gassion</aut>
          <vrk><url ref="&Monato;/2003/007934.html">Aude</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        bova <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Petro De Smet'</aut>
          <vrk><url ref="&Monato;/2008/009292.php">Sidante en pudingo, mi alvokas miajn
          savantojn!</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        de multaj jardekoj mi man&gcirc;as ekzemple malmultan <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2007/009234.php">Dikeco</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        planta <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2013/010973.php">Bombo-bankroto</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        nesaturitaj <tld/>oj de la grupo omego 6
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2013/011027.php">Oro blanka</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="buter.0o">butero</ref>,
      <ref cel="lard.0o">lardo</ref>,
      <ref cel="margar.0o">margarino</ref>,
      <ref cel="seb.0o">sebo</ref>,
      <ref cel="vegeta1.0o">vegetalino</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_l;&c_u;&c_sh;&c_ch;</trd>,
    <trd>&c_t;&c_u;&c_k;</trd>
  </trdgrp>
  <trd lng="bo">&t_zha;&t_ga;&t_tsheg;&t_tsha;&t_gigu;&t_tsheg;</trd>
  <trd lng="de">Fett</trd>
  <trd lng="en">grease</trd>
  <trd lng="es">grasa</trd>
  <trd lng="fr">graisse</trd>
  <trd lng="hu">zs&iacute;r</trd>
  <trdgrp lng="ja">
    <trd>&#33026;&#x80AA;  <pr>&#12375;&#x307C;&#12358;</pr></trd>,
    <trd>&#x6CB9;&#33026;  <pr>&#12422;&#12375;</pr></trd>,
    <trd>&#x30B0;&#x30EA;&#x30FC;&#x30B9;</trd>
  </trdgrp>
  <trd lng="nl">vet</trd>
  <trd lng="pl">t&lstroke;uszcz</trd>
  <trd lng="pt">gordura</trd>
  <trd lng="ru">&c_zh;&c_i;&c_r;</trd>
  <trd lng="tr">ya&gbreve;</trd>
  <trdgrp lng="sk">
    <trd>loj</trd>,
    <trd>olej</trd>,
    <trd>sadlo</trd>,
    <trd>tuk</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>omastek</trd>,
    <trd>rostlinn&yacute; tuk</trd>,
    <trd>s&aacute;dlo</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_zh;&c_i;&c_r;</trd>,
    <trd>&c_s;&c_a;&c_l;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6ED1;&#33026; <pr>hu&#xE1;zh&#x12B;</pr></trd>,
    <trd>&#33026;&#x80AA; <pr>zh&#x12B;f&#xE1;ng</pr></trd>,
    <trd>&#x6DA6;&#x6ED1;&#x6CB9; <pr>r&#xF9;nhu&#xE1;y&#xF3;u</pr></trd>
  </trdgrp>
