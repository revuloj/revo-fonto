      Seni&gcirc;i je <tld/>o,
      <ref tip="vid" cel="dik.mal0igxi">maldiki&gcirc;i</ref>:
      <ekz>
        vi tiel mal<tld/>i&gcirc;as
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 13:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        li povis vidi, ke li multe mal<tld/>i&gcirc;is
        <fnt>
          <aut>Lao She, trad. Wang Chongfang</aut>,
          <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0026/">
          Kamelo &Scirc;jangzi</url></vrk>,
          <lok>1988</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">abnehmen</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_u;&c_d;&c_n;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_ib;&c_z;&c_a;&c_r;&c_n;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="es">adelgazar</trd>
  <trd lng="fr">maigrir</trd>
  <trd lng="uk">&#x445;&#x443;&#x434;&#x43D;&#x443;&#x442;&#x438;</trd>
</drv>
