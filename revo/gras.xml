<drv mrk="gras.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc mrk="gras.0ajxo.io">
    <dif>
      Io <tld/>a:
      <ekz>
        man&gcirc;u <tld/>a&jcirc;on kaj trinku dol&ccirc;a&jcirc;on
        <fnt>
          <bib>MT</bib>
          <lok>Ne&hcirc;emja 8:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi havus pacon &ccirc;e via tablo, plena de <tld/>a&jcirc;oj
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 36:16</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Fettigkeit</trd>,
      <trd>Schlemmerei</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="uk">
    <trd>&#x436;&#x438;&#x440;&#x43D;&#x435;</trd>,
    <trd>&#x441;&#x43A;&#x43E;&#x440;&#x43E;&#x43C;&#x43D;&#x435; <klr>(&#x443; &#x437;&#x43D;&#x430;&#x447;. &#x456;&#x43C;.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="gras.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="gras.0igi.dikigi">
    <dif>
      Fari iun <tld/>a:
      <ekz>
        <tld/>igi porkon;
      </ekz>
      <ekz>
        la <tld/>igitan &scirc;afon vi bu&ccirc;as, sed la &scirc;afojn vi ne pa&scirc;tas
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 34:3</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_u;&c_k;&c_o;&c_r;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_d;&c_k;&c_o;&c_r;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_g;&c_l;&c_a;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">m&auml;sten</trd>
    <trd lng="es">engordar</trd>
    <trd lng="hu">h&iacute;zlal</trd>    
    <trd lng="nl">vetmesten</trd>
    <trd lng="pl">tuczy&cacute;</trd>
    <trd lng="pt">engordar</trd>
    <trd lng="ru">&c_o;&c_t;&c_k;&c_o;&c_r;&c_m;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="gras.0igi.sxmiri">
    <dif>
      Fari ion <tld/>a, &scirc;miri per <tld/>o:
      <ekz>
        <tld/>igi paton, &scirc;uojn, pafiltubon.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_t;&c_l;&c_u;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;</trd>,
      <trd>&c_sh;&c_m;&c_a;&c_l;&c_mol;&c_c;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_m;&c_a;&c_z;&c_a;&c_c;&c_mol; &c_t;&c_l;&c_u;&c_ch;&c_sh;&c_a;&c_m;</trd>
    </trdgrp>
    <trd lng="de">fetten</trd>
    <trdgrp lng="es">
      <trd>untar</trd>,
      <trd>engrasar</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>bezs&iacute;roz</trd>,
      <trd>zs&iacute;rral beken</trd>
    </trdgrp>
    <trd lng="nl">invetten</trd>
    <trdgrp lng="pl">
      <trd>nat&lstroke;uszcza&cacute;</trd>,
      <trd>okrasza&cacute;<klr> (dot. pokarm&oacute;w)</klr></trd>
    </trdgrp>
    <trd lng="pt">untar</trd>
    <trd lng="ru">
