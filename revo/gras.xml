<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>gras</rad>/o</kap>
<drv mrk="gras.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <fnt>Z</fnt>
    <dif>
      &Scirc;mirta&ubreve;ga kaj facile fandebla substanco en la korpo de
      bestoj.
    </dif>
  </snc>
  <snc>
    <dif>
      Similnatura substanco el diversa deveno:
      <ekz>
        oleo estas vegeta&jcirc;a, vazelino minerala <tld/>o.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="sub">
    <ref cel="lard.0o">lardo</ref>,
    <ref cel="seb.0o">sebo</ref>,
    <ref cel="buter.0o">butero</ref>,
    <ref cel="margar.0o">margarino</ref>,
    <ref cel="vegeta1.0o">vegetalino</ref>.
  </refgrp>
  <trd lng="de">Fett</trd>
  <trd lng="en">grease</trd>
  <trd lng="fr">graisse</trd>
  <trd lng="nl">vet</trd>
  <trd lng="ru">&c_zh;&c_i;&c_r;</trd>
</drv>

<drv mrk="gras.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Konsistanta el <tld/>o, natursimila al <tld/>o:
      <ekz>
        <tld/>a parto de viando;
      </ekz>
      <ekz>
        <ind><tld/>a makulo</ind>;
        <trd lng="nl">vetvlek</trd>
      </ekz>
      <ekz>
        <tld/>a &scirc;mira&jcirc;o.
      </ekz>
    </dif>
    <trd lng="de">fettig</trd>
    <trdgrp lng="nl">
      <trd>vet</trd>,
      <trd>vettig</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_zh;&c_i;&c_r;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_zh;&c_i;&c_r;&c_o;&c_v;&c_o;&c_j;</trd>,
      <trd>&c_s;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Havanta multe da <tld/>o, dika:
      <ekz>
        <tld/>a viando, ansero;
      </ekz>
      <ekz>
        homo tre <tld/>dika
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Ju&gcirc;istoj 3, 18</lok>)</fnt>.
      </ekz>
    </dif>
    <trd lng="de">fett</trd>
    <trd lng="nl">vet</trd>
    <trd lng="ru">&c_zh;&c_i;&c_r;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc>
    <uzo tip="fak">AGR</uzo>
    <dif>
      Bone sterkita, fruktodona:
      <ekz>
        <tld/>a grundo, kampo.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_zh;&c_i;&c_r;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_t;&c_u;&c_ch;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>gras</trd>,
    <trd>riche<klr tip="ind"> (en p. d'un sol)</klr></trd>
  </trdgrp>
  <trd lng="nl">vet</trd>
</drv>

<drv mrk="gras.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari iun <tld/>a:
      <ekz>
        <tld/>igi porkon.
      </ekz>
    </dif>
    <trd lng="de">m&auml;sten</trd>
    <trd lng="nl">vetmesten</trd>
    <trd lng="ru">&c_o;&c_t;&c_k;&c_o;&c_r;&c_m;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Fari ion <tld/>a, &scirc;miri per <tld/>o:
      <ekz>
        <tld/>igi paton, &scirc;uojn, pafiltubon.
      </ekz>
    </dif>
    <trd lng="de">fetten</trd>
    <trd lng="nl">invetten</trd>
    <trd lng="ru">&c_s;&c_m;&c_a;&c_z;&c_a;&c_t;&c_mol; &c_zh;&c_i;&c_r;&c_o;&c_m;</trd>
  </snc>
  <snc>
    <uzo tip="fak">AGR</uzo>
    <dif>
      <ref tip="dif" cel="sterk.0i">Sterki</ref>:
      <ekz>
        <tld/>igi teron, &gcirc;ardenon.
      </ekz>
    </dif>
    <trd lng="de">d&uuml;ngen</trd>
    <trd lng="nl">bemesten</trd>
    <trd lng="ru">&c_u;&c_d;&c_o;&c_b;&c_r;&c_ja;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>graisser</trd>,
    <trd>engraisser<klr tip="ind"> (tr.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="gras.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      I&gcirc;i <tld/>a:
      <ekz>
        la porkoj tre facile <tld/>i&gcirc;as;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la akciuloj <tld/>i&gcirc;as <klr>(ri&ccirc;i&gcirc;as)</klr> per
        altaj dividendoj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>gossir</trd>,
    <trd>engraisser<klr tip="ind"> (intr.)</klr></trd>
  </trdgrp>
  <trd lng="nl">vet worden</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_i;&c_r;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_t;&c_u;&c_ch;&c_n;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="gras.0igajxo">
  <kap><tld/>iga&jcirc;o</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <ref tip="dif" cel="sterk.0o">sterko</ref>
  </snc>
  <trd lng="fr">engrais</trd>
  <trd lng="nl">mest</trd>
  <trd lng="ru">&c_u;&c_d;&c_o;&c_b;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="gras.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Havanta malmulte da <tld/>o:
      <ekz>
        la sep malgrasaj bovinoj el la Biblio.
      </ekz>
    </dif>
    <trd lng="nl">mager</trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_o;&c_shch;&c_i;&c_j;</trd>,
      <trd>&c_h;&c_u;&c_d;&c_o;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">AGR</uzo>
    <dif>
      Malmulte fruktodona, malbone sterkita:
      <ekz>
        mal<tld/>a grundo
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ricx.mal0a">malri&ccirc;a</ref>.
    </refgrp>
  </snc>
  <trd lng="de">mager</trd>
  <trdgrp lng="fr">
    <trd>maigre</trd>,
    <trd>pauvre<klr tip="ind"> (en p. d'un sol)</klr></trd>
  </trdgrp>
  <trd lng="nl">arm<klr tip="ind"> (mager)</klr></trd>
</drv>
</art>
