    <trd>uty&cacute;</trd>,
    <trd>rozty&cacute; si&eogonek;</trd>,
    <trd>tuczy&cacute; si&eogonek;</trd>,
    <trd>utuczy&cacute; si&eogonek;</trd>
  </trdgrp>
  <trd lng="pt">engordar<klr tip="amb"> (tornar-se gordo)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_i;&c_r;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_t;&c_u;&c_ch;&c_n;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="tr">&scedil;i&scedil;manlamak</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_p;&c_r;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_g;&c_l;&c_a;&c_d;&c_sh;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_o;&c_g;&c_l;&c_a;&c_d;&c_sh;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_u;&c_d;&c_o;&c_b;&c_r;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

