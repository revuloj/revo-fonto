<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: krokod1.xml,v 1.10 2019/03/22 18:10:14 revo Exp $">
<kap>
  <rad>krokodiled</rad>/oj
</kap>
<drv mrk="krokod1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt"
      cel="krokod.0uloj">krokodiluloj</ref>
      (<trd lng="la">Crocodylidae</trd>), &ccirc;e kiuj la kvara dento estas
      videbla &ccirc;e fermita bu&scirc;o:
      <ekz>
        <tld/>oj diferencas de la <ref cel="aligat1.0oj">aligatoredoj</ref>
        unuavice la&ubreve; la dentoj, kiam la makzeloj de
        <ref tip="prt" cel="krokod.0o">krokodilo</ref> estas fermitaj, estas
        videbla la granda kvara dento <klr>[&#8230;]</klr>
        dum &ccirc;e aligatoro la supra makzelo kovras &ccirc;iujn dentojn
        <fnt>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="reu.ru/prenu/rego70_71reta.pdf"
              >Mallonga populara krokodilologio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_p;&c_r;&c_a;&c_w;&c_d;&c_n;&c_y;&c_ja; <ind>&c_k;&c_r;&c_a;&c_k;&c_a;&c_d;&c_z;&c_ib;&c_l;&c_y;</ind> <klr
  tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Echte <ind>Krokodile</ind></trd>
  <trd lng="hu">krokodilf&eacute;l&eacute;k</trd>
  <trd lng="pl">krokodylowate</trd>
  <trd lng="pt">crocodil&iacute;deos</trd>
  <trd lng="ro">crocodylidae</trd>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_i;&c_je; <ind>&c_k;&c_r;&c_o;&c_k;&c_o;&c_d;&c_i;&c_l;&c_y;</ind> <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>

