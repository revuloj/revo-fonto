<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: poloni.xml,v 1.21 2024/02/17 07:52:34 revo Exp $">
<kap>
  <rad>poloni</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="poloni.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Po</mlg>
  <snc mrk="poloni.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="84">elemento</ref> kun
      atomnumero 84, atompezo 208,9824; kies &ccirc;iuj 
      <ref tip="super" cel="izotop.0o">izotopoj</ref> 
      estas radioaktivaj:
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> estis malkovrita de <nom>Maria Sk&lstroke;odowska-Curie</nom> kaj <nom>Pierre
        Curie</nom> en 1898 kaj poste estis nomita pro la denaska lando de <nom>Maria</nom>, Pollando
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=27900">Polonio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_jo;&c_n;&c_ib;&c_j;</trd>   
  <trd lng="bg">&c_p;&c_o;&c_l;&c_o;&c_n;&c_i;&c_j;</trd>     
  <trd lng="br">poloniom</trd>
  <trd lng="de">Polonium</trd>
  <trd lng="en">polonium</trd>
  <trd lng="es">polonio</trd>
  <trd lng="fa">&f_p;&a_w;&a_l;&a_w;&a_n;&f_y;&a_m;</trd>
  <trd lng="fr">polonium</trd>
  <trd lng="hu">pol&oacute;nium</trd>
  <trd lng="id">polonium</trd>
  <trd lng="it">polonio</trd>
  <trd lng="ja">&#x30DD;&#x30ED;&#x30CB;&#x30A6;&#x30E0;</trd>
  <trd lng="la">Polonium</trd>
  <trd lng="nl">polonium</trd>
  <trd lng="pl">polon</trd>
  <trd lng="pt">pol&ocirc;nio</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_o;&c_n;&c_i;&c_j;</trd>
  <trd lng="sv">polonium</trd>
  <trd lng="uk">&#x43F;&#x43E;&#x43B;&#x43E;&#x43D;&#x456;&#x439;</trd>
  <trdgrp lng="zh">
    <trd>&#x91D9; <pr>p&#x14D;</pr></trd>,
    <trd>&#x948B; <pr>p&#x14D;</pr></trd>
  </trdgrp>
</drv>
</art>
