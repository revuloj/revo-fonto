<vortaro>
<art mrk="$Id$">
<kap>
  <rad>poloni</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="poloni.0o">
  <kap><tld/>o</kap>
  <mlg>Po</mlg>
  <snc mrk="poloni.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 84, atompezo 208,9824, simbolo <frm>Po</frm>.
      &Ccirc;iuj <ref tip="super" cel="izotop.0o">izotopoj</ref> de tiu
      elemento estas radioaktivaj.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_jo;&c_n;&c_ib;&c_j;</trd>   
  <trd lng="bg">&c_p;&c_o;&c_l;&c_o;&c_n;&c_i;&c_j;</trd>     
  <trd lng="br">poloniom</trd>
  <trd lng="de">Polonium</trd>
  <trd lng="en">polonium</trd>
  <trd lng="es">polonio</trd>
  <trd lng="fa">&f_p;&a_w;&a_l;&a_w;&a_n;&f_y;&a_m;</trd>
  <trd lng="fr">polonium</trd>
  <trd lng="hu">pol&oacute;nium</trd>
  <trd lng="it">polonio</trd>
  <trd lng="la">Polonium</trd>
  <trd lng="nl">polonium</trd>
  <trd lng="pl">polon</trd>
  <trd lng="pt">pol&ocirc;nio</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_o;&c_n;&c_i;&c_j;</trd>
  <trd lng="sv">polonium</trd>
</drv>
</art>
