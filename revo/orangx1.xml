        &scirc;i havis <klr>...</klr> belegan &gcirc;ardenon kun <tld/>oj
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 15a, p.
        80a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_r;&c_a;&c_n;&c_zh;&c_a;&c_r;&c_e;&c_ja;</trd>,
    <trd>&c_c;&c_ja;&c_p;&c_l;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
