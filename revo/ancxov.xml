  </snc>
  <trd lng="de">Anchovis</trd>
  <trd lng="fr">anchois</trd>
  <trd lng="nl">anchovis</trd>
  <trd lng="ru">&#1072;&#1085;&#1095;&#1086;&#1091;&#1089;</trd>
