<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: templ.xml,v 1.35 2024/01/16 21:22:48 revo Exp $">
    <kap>
      <ofc>1</ofc>
      <rad>templ</rad>/o
    </kap>

    <drv mrk="templ.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="templ.0o.rezidejo">
        <dif>
          Konstrua&jcirc;o destinita por lo&gcirc;ado, kulto a&ubreve;
              servado de dio a&ubreve; diino:
          <ekz>
            ne malproksime de la urbo Pi-Bast estis la <tld/>o de
            l' diino Hator
            <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 1.</lok></fnt>;
          </ekz><ekz>
            e&ccirc; la vazojn de la domo de Dio, la orajn kaj
                ar&gcirc;entajn,
            kiujn Nebukadnecar elportis el la <tld/>o de Jerusalem
            kaj forportis en la <tld/>on de Babel,
            ilin la re&gcirc;o Ciro elportis el la <tld/>o de Babel
            <fnt>
              <bib>MT</bib>, <lok>&Ezr; 5:14.</lok>
            </fnt>.
          </ekz>
        </dif>
        <trd lng="de">Tempel</trd>
          </snc>
          <snc mrk="templ.0o.kultejo">
        <dif>
         Kultejo, konstrua&jcirc;o en kiu samkultanoj plenumas siajn ritojn:
          <ekz>
            protestanta, hindisma, framasona, homaranisma <tld/>o.
          </ekz>
        </dif>
        <ref tip="sin" cel="pregx.0ejo">pre&gcirc;ejo</ref>
        <trd lng="de">Tempel</trd>
          </snc>
          <snc mrk="templ.0o.FIG">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Restadejo de ia sankta a&ubreve; nobla a&ubreve; abstrakta afero:
          <ekz>
            via korpo estu <tld/>o de l' animo;
          </ekz><ekz>
            <tld/>o de l' arto.
          </ekz>
        </dif>
        <trd lng="de">Tempel (-ruine)</trd>
      </snc>
  <trd lng="be">&c_h;&c_r;&c_a;&c_m;</trd>
  <trd lng="bg">&c_h;&c_r;&c_a;&c_m;</trd>
  <trd lng="bo">&#xF63;&#xFB7;&#xF0B;&#xF41;&#xF44;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>chr&aacute;m</trd>,
    <trd>posv&aacute;tn&eacute; m&iacute;sto</trd>,
    <trd>synagoga</trd>,
    <trd>templ</trd>
  </trdgrp>
  <trd lng="de">Tempel</trd>
  <trd lng="es">templo</trd>
  <trdgrp lng="fa">
        <trd>&f_p;&a_r;&a_s;&a_t;&a_sh;&f_g;&a_A;&a_h;</trd>,
        <trd>&a_m;&a_ayn;&a_b;&a_d;</trd>
      </trdgrp>
  <trd lng="fr">temple</trd>
  <trd lng="hi">&#x92E;&#2306;&#2342;&#x93F;&#2352; [<ind>ma&#x1E41;dir</ind>]</trd>
  <trd lng="hu">templom</trd>
  <trd lng="it">tempio</trd>
  <trdgrp lng="ja">
    <trd>&#x795E;&#x6BBF;  <pr>&#12375;&#12435;&#12391;&#12435;</pr></trd>,
    <trd>&#x5BFA;&#38498;  <pr>&#12376;&#12356;&#12435;</pr></trd>,
    <trd>&#32854;&#22530;  <pr>&#x305B;&#12356;&#12393;&#12358;</pr></trd>,
    <trd>&#x793C;&#x62DD;&#22530;  <pr>&#x308C;&#12356;&#x306F;&#12356;&#12393;&#12358;</pr></trd>,
    <trd>&#25945;&#x4F1A;&#22530;  <pr>&#x304D;&#12423;&#12358;&#x304B;&#12356;&#12393;&#12358;</pr></trd>,
    <trd>&#x6BBF;&#22530;  <pr>&#12391;&#12435;&#12393;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">tempel</trd>
  <trdgrp lng="pl">
        <trd>&sacute;wi&aogonek;tynia</trd>,
        <trd>chram<klr> (przest.)</klr></trd>
      </trdgrp>
  <trdgrp lng="pt">
        <trd>templo</trd>
      </trdgrp>
  <trd lng="ru">&c_h;&c_r;&c_a;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>chr&aacute;m</trd>,
    <trd>posv&auml;tn&eacute; miesto</trd>
  </trdgrp>
  <trd lng="uk">&c_h;&c_r;&c_a;&#x43C;</trd>
  <trdgrp lng="zh">
    <trd>&#x6BBF;<klr> [di&agrave;n]</klr></trd>,
    <trd>&#x5E99;<klr> [mi&agrave;o]</klr></trd>,
    <trd>&#x5BFA;&#xFF08;&#x4F5B;&#x5BFA;&#xFF09;<klr>[s&igrave;]</klr></trd>
  </trdgrp>
</drv>

    <drv mrk="templ.En0igo">
      <kap>En<tld/>igo</kap>
      <snc>
        <uzo tip="fak">KRI</uzo>
        <dif>
          En la tradicia (ortodoksa, katolika) kristanismo,
          la <ref tip="dif" cel="fest.dipatrina0o">dipatrina festo
          </ref> (unu el la
          <ref tip="dif" cel="fest.dekduopa0o">dekduopaj
          festoj</ref>)
          per kiu la Eklezio rememoras enkondukon de la sanktega
          virgulino Mario en la Templon (je la a&gcirc;o de 3 jaroj) por
          edukado &ccirc;e la Templo.
        </dif>
      </snc>
      <trd lng="en">Presentation<klr> of the Blessed Virgin Mary</klr></trd>
      <trd lng="pl">Ofiarowanie Naj&sacute;wi&eogonek;tszej Maryi Panny</trd>
      <trdgrp lng="pt">
        <trd>Apresenta&ccedil;&atilde;o<klr tip="amb">
        (da Virgem Maria)</klr></trd>
      </trdgrp>
      <trd lng="ru">&c_V;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_k; &c_v;&c_o; &c_h;&c_r;&c_a;&c_m;<klr>  (&c_p;&c_r;&c_je;&c_s;&c_v;&c_ja;&c_t;&c_o;&c_j; &c_B;&c_o;&c_g;&c_o;&c_r;&c_o;&c_d;&c_i;&c_c;&c_y;)</klr></trd>
    </drv>
  </art>
