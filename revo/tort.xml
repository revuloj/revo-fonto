<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tort.xml,v 1.26 2024/10/20 16:31:57 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tort</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="tort.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tort.0o.KUI">
  <uzo tip="fak">KUI</uzo>
    <dif>
      Baka&jcirc;o el ordinare rondforma fundo kun diversspeca garna&jcirc;o
      (fruktoj, krema&jcirc;o, legomoj):
      <ekz>
        geedzi&gcirc;a <tld/>o
        <fnt>
          <bib>Kon11</bib>
          <lok>Pe&ccirc;aku&ccirc;o 20x20</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;erizo sur la <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Petro De Smet'</aut>
          <vrk><url ref="&Monato;/2011/010160.php"
              >Belaj figoj post Pasko</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Olga</nom> <klr>[&#8230;]</klr> festis sian naski&gcirc;tagon kaj senavare dividis la festan
        <tld/>on
        <fnt>
          <bib>Kon11</bib>
          <lok>Informe</lok>
        </fnt>;
      </ekz>
      <ekz>
        granda apuda tablo estis plene kovrita per bongustegaj
        fruktoj kaj nuksoj, <tld/>oj kaj kukoj
        <fnt>
          <aut>L. Frank Baum, trad. D. Broadribb</aut>
          <vrk>La Mirinda Sor&ccirc;isto de Oz</vrk>
         <lok>&ccirc;apitro 3a</lok>
        </fnt>;
      </ekz>
      <ekz>
        fari migdalan <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Istv&aacute;n Ertl</aut>
          <vrk><url ref="&Monato;/2013/010910.php"
              >Dobos Delight</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        batita laktokremo, per kiu oni ornamas la faman Sa&hcirc;er-<tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk>Batita kremo kaj akvo&jcirc;etiloj</vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="kisx.0o">ki&scirc;o</ref>
    <refgrp tip="vid">
      <ref cel="flan.0o">flano</ref>,
      <ref cel="pastec.0o">paste&ccirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_o;&c_r;&c_t;</trd>
  <trd lng="ca">coca</trd>
  <trd lng="cs">dort</trd>
  <trd lng="de">Torte</trd>
  <trdgrp lng="en">
     <trd>tart</trd>,
     <trd>pie</trd>
  </trdgrp>
  <trd lng="es">torta<klr> (reposter&iacute;a)</klr></trd>
  <trdgrp lng="fr">
    <trd>tarte</trd>,
    <trd>tourte</trd>
  </trdgrp>
  <trd lng="hu">torta</trd>
  <trd lng="ja">&#12479;&#12523;&#12488;</trd>
  <trd lng="nl">taart</trd>
  <trd lng="pl">tort</trd>
  <trd lng="pt">torta</trd>
  <trd lng="ru">&c_t;&c_o;&c_r;&c_t;</trd>
  <trd lng="sk">torta</trd>
  <trd lng="uk">&c_t;&c_o;&c_r;&c_t;</trd>
</drv>

<drv mrk="tort.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="tort.0eto.KUI">
  <uzo tip="fak">KUI</uzo>
    <dif>
      Unuporcia malgranda <tld/>o:
      <ekz>
        sekfruktaj <tld/>etoj
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#8470;1</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Elly</nom>, la <tld/>etoj vere estas frandindaj
        <fnt>
          <aut>L. Couperus, trad. G. Berveling</aut>,
          <vrk>Pri maljunuloj, la a&jcirc;oj kiuj pasas...</vrk>,
          <lok>2013</lok>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_r;&c_o;&c_zh;&c_n;&c_a;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>mou&ccaron;n&iacute;k</trd>,
    <trd>z&aacute;kusek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>T&ouml;rtchen</trd>,
    <trd>Tortelette</trd>
  </trdgrp>
  <trd lng="fr">tartelette</trd>
  <trd lng="pl">torcik</trd>
  <trdgrp lng="sk">
    <trd>m&uacute;&ccaron;nik</trd>,
    <trd>z&aacute;kusok</trd>
  </trdgrp>
  <trd lng="uk">&c_t;&c_ib;&c_s;&c_t;&c_je;&c_ch;&c_k;&c_o;</trd>
</drv>
</art>
