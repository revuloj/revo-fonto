<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>zebu</rad>/o
</kap>
<drv mrk="zebu.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="bov.0o">bovo</ref>
      (<trd lng="la">Bos mutus</trd>), kun &gcirc;ibo,
      bredata en Hindujo kaj najbaraj landoj kiel hejmbesto.
    </dif>
  </snc>
  <trdgrp lng="de"><trd>Zebu</trd>, <trd>Buckelrind</trd></trdgrp>
</drv>
</art>
