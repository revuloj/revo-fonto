<art mrk="$Id: trut.xml,v 1.21 2023/09/04 19:01:54 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>trut</rad>/o
</kap>
<drv mrk="trut.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
       <ref tip="lst" lst="voko:zoologiaj_specioj" cel="speci.0o">Specio</ref>
       de <ref tip="super" cel="salm.0o">salmo</ref>
       (<trd lng="la">Salmo trutta</trd>),
       tre delikatgusta <ref tip="lst" lst="voko:fi&scirc;oj" cel="fisx.river0o">riverfi&scirc;o</ref>:
      <ekz>
        ekzistas du fi&scirc;etoj: mareno kaj <tld/>o, tiel bongustaj, ke oni lekas al si la fingrojn, kiam oni ilin man&gcirc;as
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        norvegoj produktas pli ol 700&#x202F;000 tunojn da salmo, 70&#x202F;000 tunojn da <tld/>o, 
        malpli grandajn kvantojn da moruo, angilo, aliaj fi&scirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2015/011433.php">Krozi la&ubreve; la norvega marbordo</url></vrk>
        </fnt>.
      </ekz>
     </dif>
   </snc>
   <bld lok="../bld/trut.jpg">
     kaptita sova&gcirc;a <tld/>o </bld>
   <trd lng="be">&c_s;&c_t;&c_r;&c_o;&c_n;&c_g;&c_a;</trd>
   <trd lng="de">Forelle</trd>
   <trd lng="en">trout</trd>
   <trd lng="fr">truite</trd>
   <trd lng="hu">pisztr&aacute;ng</trd>
   <trdgrp lng="ja">
    <trd>&#x30DE;&#x30B9;</trd>,
    <trd>&#x30D6;&#x30E9;&#x30A6;&#x30F3;&#x30C8;&#x30E9;&#x30A6;&#x30C8;</trd>,
    <trd>&#x30CB;&#x30B8;&#x30DE;&#x30B9;</trd>
  </trdgrp>
  <trd lng="nl">forel</trd>
   <trd lng="pl">pstr&aogonek;g</trd>
   <trd lng="pt">truta</trd>
   <trd lng="ru">&c_f;&c_o;&c_r;&c_je;&c_l;&c_mol;</trd>
   <trd lng="sv">&ouml;ring</trd>
  <trd lng="sk">pstruh</trd>
  <trd lng="cs">pstruh</trd>
  <trdgrp lng="uk">
    <trd>&#x444;&#x43E;&#x440;&#x435;&#x43B;&#x44C;</trd>,
    <trd>&#x43B;&#x43E;&#x441;&#x43E;&#x441;&#x44C; &#x43A;&#x443;&#x43C;&#x436;&#x430;</trd>,
    <trd>&#x43A;&#x443;&#x43C;&#x436;&#x430;</trd>
  </trdgrp>
</drv>
</art>
