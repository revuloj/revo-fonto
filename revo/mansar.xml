<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mansar.xml,v 1.26 2024/02/27 09:23:43 revo Exp $">
<kap>
   <ofc>2</ofc>
   <rad>mansard</rad>/o
   <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="mansar.0o">
   <kap>
     <tld/>o
   </kap>
   <snc>
     <uzo tip="fak">ARKI</uzo>
     <dif>
      Subtegmenta <ref tip="super" cel="cxambr.0o.doma">&ccirc;ambro</ref>
      ekipita por lo&gcirc;ado:
      <ekz>
        la bruo de la strato <klr>[&#8230;]</klr> obtuze kaj neklare atingadis la <tld/>on
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> restis en la <tld/>o kaj ser&ccirc;is distron en siaj romanoj
        <fnt>
          <bib>HsT</bib>
          <lok>Heredanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li iris supren en la <tld/>on, kie li eksidis &ccirc;e la fenestro kaj rigardis
        okcidenten
        <fnt>
          <bib>Tor</bib>
          <lok>II.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[tiuj vortoj]</klr> restis (mi ne scias kiel) en la polvokovrita <tld/>o de
        mia menso
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2005/008548.php">Lingva atavismo</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="tegmen.sub0o">subtegmento</ref>
   </snc>
   <trd lng="be">&c_m;&c_a;&c_n;&c_s;&c_a;&c_r;&c_d;&c_a;</trd>
   <trd lng="br">ma&ntilde;sardenn</trd>
   <trd lng="ca">golfes</trd>
   <trdgrp lng="de">
     <trd>Mansarde</trd>,
     <trd>Dachstube</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>attic</trd>,
     <trd>garret</trd>
   </trdgrp>
   <trdgrp lng="es">
     <trd>&aacute;tico</trd>,
     <trd>mansarda</trd>
   </trdgrp>
   <trd lng="fa">&a_A;&a_t;&a_A;&a_q; &a_z;&f_y;&a_r;&a_sh;&f_y;&a_r;&a_w;&a_A;&a_n;&f_y;</trd>
   <trd lng="fr">mansarde</trd>
   <trdgrp lng="hu">
     <trd>padl&aacute;sszoba</trd>,
     <trd>manz&aacute;rd</trd>
   </trdgrp>
   <trdgrp lng="ja">
    <trd>&#x30DE;&#x30F3;&#x30B5;&#x30FC;&#x30C9;&#x5C4B;&#26681; 
    <pr>&#x30DE;&#x30F3;&#x30B5;&#x30FC;&#x30C9;&#12420;&#x306D;</pr></trd>,
    <trd>&#x5C4B;&#26681;&#x88CF;&#x90E8;&#x5C4B;  <pr>&#12420;&#x306D;&#12358;&#12425;&#12409;&#12420;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
     <trd>mansarde</trd>,
     <trd>zolderkamer</trd>
   </trdgrp>
   <trd lng="pl">mansarda</trd>
   <trdgrp lng="pt">
     <trd>mansarda</trd>,
     <trd>s&oacute;t&atilde;o</trd>
   </trdgrp>
   <trd lng="ro">mansard&abreve;</trd>
   <trd lng="ru">&c_m;&c_a;&c_n;&c_s;&c_a;&c_r;&c_d;&c_a;</trd>
   <trd lng="sv"><klr>(hus-)</klr>vind</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_a;&c_n;&c_s;&c_a;&c_r;&c_d;&c_a;</trd>,
    <trd>&c_g;&c_o;&c_r;&c_i;&c_shch;&c_n;&c_je; &c_p;&c_r;&c_i;&c_m;&c_ib;&c_shch;&c_je;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#38914;&#x6A13; <pr>d&#x1D0;ngl&#xF3;u</pr></trd>,
    <trd>&#39030;&#x697C; <pr>d&#x1D0;ngl&#xF3;u</pr></trd>
  </trdgrp>
</drv>
</art>
