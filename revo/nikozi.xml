<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: nikozi.xml,v 1.8 2024/01/29 15:59:10 afido Exp $">
    <kap>
      <rad>Nikozi</rad>/o, <var><kap><rad var="s">Nikosi</rad>/o</kap></var>
    </kap>

<drv mrk="nikozi.0o">
  <kap><tld/>o, <var><kap><tld var="s"/>o</kap></var></kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      La <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&ccirc;efurbo</ref>
      de <ref tip="malprt" cel="kipr.K0o.sxtato">Kipro</ref>:
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> situas en la centro de la insulo
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Nikozio">Nikozio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        transporti varojn <klr>[&#x2026;]</klr> al la &ccirc;efurbo <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2011/010348.php">La&ubreve; linioj de la pasinteco</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la &ccirc;efurbo <tld var="s"/>o <klr>[&#x2026;]</klr>
        oni publikigis deklarojn pri la estonteco de Kipro
        <fnt>
          <vrk><url ref="&Monato;/2002/007899.html">&Ccirc;u dividita insulo dividos E&ubreve;ropon?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Nikosia</trd>
    <trd lng="en">Nikosia</trd>
    <trd lng="fr">Nikosie</trd>
    <trd lng="pl">Nikozja</trd>
    <trd lng="ro">Nicosia</trd>
  </snc>
  <trd lng="be">&c_N;&c_ib;&c_k;&c_a;&c_s;&c_ib;&c_ja;</trd>
  <trd lng="ja">&#x30CB;&#x30B3;&#x30B7;&#x30A2;</trd>
  <trd lng="uk">&#x41D;&c_ib;&#x43A;&#x43E;&c_z;&c_ib;&#x44F; (&c_s;&c_t;&#x43E;&#x43B;&c_i;&c_c;&#x44F;)</trd>
  <trdgrp lng="zh">
    <trd>&#x5C3C;&#x79D1;&#x897F;&#x4E9A; <pr>n&#xED;k&#x113;x&#x12B;y&#xE0;</pr></trd>,
    <trd>&#x5C3C;&#x79D1;&#x897F;&#x4E9E; <pr>n&#xED;k&#x113;x&#x12B;y&#xE0;</pr></trd>
  </trdgrp>
</drv>

