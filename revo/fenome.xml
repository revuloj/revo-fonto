<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>fenomen</rad>/o
</kap>
<drv mrk="fenome.0o">
  <kap><tld/>o</kap>
  <snc mrk="fenome.0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Perceptebla per la sentumoj apero; natura okaza&jcirc;o:
      <ekz>
        astronomiaj, fizikaj, spiritaj <tld/>oj.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Ph&auml;nomen</trd>,
      <trd>Erfahrungstatsache</trd>
    </trdgrp>
    <trd lng="fr">ph&eacute;nom&egrave;ne</trd>
    <trdgrp lng="hu">
      <trd>jelens&eacute;g</trd>,
      <trd>t&uuml;nem&eacute;ny</trd>
    </trdgrp>
  </snc>
  <snc mrk="fenome.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malofta, eksterordinara, stranga afero:
      <ekz>
        renkonti vin estas vera <tld/>o<fnt>B</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="event.0o">evento</ref>,
      <ref cel="monstr.0o.FIG">monstro <sncref/></ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Ph&auml;nomen</trd>,
      <trd>seltenes <ind>Ereignis</ind></trd>,
      <trd>seltsame <ind>Sache</ind></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>ph&eacute;nom&egrave;ne<klr> (&eacute;v&egrave;nement)</klr></trd>,
      <trd>merveille<klr> (&eacute;v&egrave;nement)</klr></trd>,
      <trd>prodige<klr> (&eacute;v&egrave;nement)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>ritka <ind>dolog</ind></trd>,
      <trd>rendk&iacute;v&uuml;li <ind>eset</ind></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_e;&c_n;&c_o;&c_m;&c_e;&c_n;</trd>,
    <trd>&c_z;&c_mol;&c_ja;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="nl">fenomeen</trd>
  <trd lng="es">fen&oacute;meno</trd>
  <trd lng="pt">fen&#244;meno</trd>
  <trdgrp lng="ru">
    <trd>&c_ja;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_f;&c_je;&c_n;&c_o;&c_m;&c_je;&c_n;</trd>
  </trdgrp>
</drv>

<drv mrk="fenome.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Malofta, stranga, eksterordinara:
      <ekz>
        <tld/>a aventuro, floro, estulo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_e;&c_n;&c_a;&c_m;&c_e;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trd lng="fr">ph&eacute;nom&eacute;nal</trd>
  <trdgrp lng="hu">
    <trd>t&uuml;nem&eacute;nyes</trd>,
    <trd>nem mindennapi</trd>
  </trdgrp>
  <trd lng="nl">fenomenaal</trd>
  <trd lng="es">fenomenal</trd>
  <trd lng="ru">&c_f;&c_je;&c_n;&c_o;&c_m;&c_je;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
