<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fenome.xml,v 1.29 2023/11/09 19:13:16 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>fenomen</rad>/o
</kap>

<drv mrk="fenome.0o">
  <kap><tld/>o</kap>
  <snc mrk="fenome.0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Perceptebla per la sentumoj <ref tip="super" cel="aper.0ajxo.fenomeno">apero</ref>; 
      natura <ref cel="okaz.0o.travivajxo">okazo</ref>:
      <ekz>
        ni kunmetas la kalendaron kaj anta&ubreve;kalkulas la estontajn <tld/>ojn sur la &ccirc;ielo
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        montri&gcirc;is la Fatamorgano, tiu belega aera <tld/>o de la dezertoj, kun &scirc;vebantaj
        &gcirc;ardenoj kaj na&gcirc;antaj arbaroj
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu, por klarigi la <tld/>on, elpensis sian teorion
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni klarigas tiaspecajn aerajn <tld/>ojn per norda lumo, <klr>[&#x2026;]</klr> kredeble
        ili naski&gcirc;as per la elektra forto
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        por bone kompreni la <tld/>ojn en la naturo kaj en la socio, oni devas rigardi
        ilin ne kiel senmovajn statojn, sed kiel evoluprocezojn
        <fnt>
          <bib>EeP</bib>
          <lok>Unua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="procez.0o.SCI">procezo<sncref/></ref>
    <trd lng="de">Erfahrungstatsache</trd>
    <trd lng="fa">&f_p;&a_d;&f_y;&a_d;&a_h;</trd>
    <trd lng="fr">ph&eacute;nom&egrave;ne</trd>
    <trdgrp lng="hu">
      <trd>jelens&eacute;g</trd>,
      <trd>t&uuml;nem&eacute;ny</trd>
    </trdgrp>
  </snc>
  <snc mrk="fenome.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malofta, eksterordinara, stranga <ref tip="super" cel="afer.0o.cxio">afero<sncref/></ref>:
      <ekz>
        jen la arbeta&jcirc;o brulas en la fajro, kaj tamen la arbeta&jcirc;o ne forbrulas
        <klr>[&#x2026;]</klr> mi iros kaj rigardos tiun grandan <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 3:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        apud la piramido <nom>Ko&hcirc;om</nom> oni rimarkis miraklajn <tld/>ojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        mistera murmureto eksonis kaj la lumo ekbrilis, la <tld/>o da&ubreve;ris tri sekundojn
        <fnt>
          <bib>IKr</bib>
          <lok>Rakonto Pri Spiritoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i estas <klr>[&#x2026;]</klr> <tld/>o de la plej alta moraleco
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XIII</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="event.0o">evento</ref>,
      <ref cel="monstr.0o.FIG">monstro<sncref/></ref>,
      <ref cel="mirakl.0o">miraklo</ref>,
      <ref cel="mister.0o">mistero</ref>,
      <ref cel="sensac1.0o">sensacio</ref>,
      <ref cel="strang.0ajxo">stranga&jcirc;o</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>seltsame <ind>Sache</ind></trd>,
      <trd>seltenes <ind>Ereignis</ind></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>ph&eacute;nom&egrave;ne<klr>
      (&eacute;v&egrave;nement)</klr></trd>,
      <trd>merveille<klr> (&eacute;v&egrave;nement)</klr></trd>,
      <trd>prodige<klr> (&eacute;v&egrave;nement)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>ritka <ind>dolog</ind></trd>,
      <trd>rendk&iacute;v&uuml;li <ind>eset</ind></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_e;&c_n;&c_o;&c_m;&c_e;&c_n;</trd>,
    <trd>&c_z;&c_mol;&c_ja;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_ja;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_f;&c_je;&c_n;&c_o;&c_m;&c_je;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>efekt</trd>,
    <trd>fenom&eacute;n</trd>,
    <trd>jev</trd>
  </trdgrp>
  <trd lng="de">Ph&auml;nomen</trd>
  <trd lng="es">fen&oacute;meno</trd>
  <trdgrp lng="ja">
    <trd>&#x73FE;&#x8C61;  <pr>&#x3052;&#x3093;&#x3057;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x4E8B;&#x8C61;  <pr>&#x3058;&#x3057;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x7279;&#x7570;&#x73FE;&#x8C61;  <pr>&#x3068;&#x304F;&#x3044;&#x3052;&#x3093;&#x3057;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x9A5A;&#x304F;&#x3079;&#x304D;&#x4E8B;&#x4EF6;  <pr>&#x304A;&#x3069;&#x308D;&#x304F;&#x3079;&#x304D;&#x3058;&#x3051;&#x3093;</pr></trd>,
    <trd>&#x5916;&#x8C61;  <pr>&#x304C;&#x3044;&#x305E;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">fenomeen</trd>
  <trd lng="pl">fenomen</trd>
  <trd lng="pt">fen&ocirc;meno</trd>
  <trdgrp lng="ru">
    <trd>&c_ja;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_f;&c_je;&c_n;&c_o;&c_m;&c_je;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>jav</trd>,
    <trd>zjav</trd>,
    <trd>&uacute;kaz</trd>
  </trdgrp>
  <trd lng="tr">fenomen</trd>
  <trdgrp lng="uk">
    <trd>&c_ja;&c_v;&c_i;&c_shch;&c_je;</trd>,
    <trd>&c_f;&c_je;&c_n;&c_o;&c_m;&c_je;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x73B0;&#x8C61; <pr>xi&#xE0;nxi&#xE0;ng</pr></trd>,
    <trd>&#x7269;&#x5019; <pr>w&#xF9;h&#xF2;u</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fenome.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <ref cel="oft.mal0a">Malofta</ref>, <ref tip="super" cel="strang.0a">stranga</ref>, 
      <ref tip="sin" cel="ordina.ekster0a">eksterordinara</ref>:
      <ekz>
        jen mi staras kun miaj <tld/>aj kapabloj
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova strata lanterno</lok>
        </fnt>!
      </ekz>
      <ekz>
        <tld/>a spekta&jcirc;o
        <fnt>
          <bib>ChM</bib>
          <lok>3</lok>
        </fnt>;
      </ekz>
      <ekz>
        samtempuloj aprezis anka&ubreve; la <tld/>an memorkapablon de sia reganto
        <fnt>
          <bib>Monato</bib>
          <aut>Vitalij kaj Anatolij Ionesov</aut>
          <vrk><url ref="&Monato;/2010/009996.php">Mirzo Ulugbek: granda re&gcirc;o de astronomio</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fantast.0a">fantasta</ref>,
      <ref cel="mir.0inda">mirinda</ref>,
      <ref cel="kred.ne0ebla">nekredebla</ref>,
      <ref cel="origin1.0a.stranga">originala<sncref/></ref>,
      <ref cel="rar.0a.malofta">rara<sncref/></ref>,
      <ref cel="unik.0a.eksterordinara">unika<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_f;&c_e;&c_n;&c_a;&c_m;&c_e;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trdgrp lng="bg">
    <trd>&c_f;&c_je;&c_n;&c_o;&c_m;&c_je;&c_n;&c_a;&c_l;&c_je;&c_n;</trd>,
    <trd>&c_i;&c_z;&c_k;&c_l;&c_ju;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_je;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>fenomen&aacute;ln&iacute;</trd>,
    <trd>fenom&eacute;nov&yacute;</trd>,
    <trd>nev&scaron;edn&iacute;</trd>,
    <trd>t&yacute;kaj&iacute;c&iacute; se fenom&eacute;nu</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ph&auml;nomenal</trd>,
    <trd>phantastisch</trd>
  </trdgrp>
  <trd lng="es">fenomenal</trd>
  <trd lng="fa">&a_kh;&a_A;&a_r;&a_q;&zwnj;&a_A;&a_l;&a_ayn;&a_A;&a_d;&a_h;</trd>
  <trd lng="fr">ph&eacute;nom&eacute;nal</trd>
  <trdgrp lng="hu">
    <trd>t&uuml;nem&eacute;nyes</trd>,
    <trd>nem mindennapi</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x73FE;&#x8C61;&#x306E;  <pr>&#x3052;&#x3093;&#x3057;&#x3087;&#x3046;&#x306E;</pr></trd>,
    <trd>&#x9A5A;&#x304F;&#x3079;&#x304D;  <pr>&#x304A;&#x3069;&#x308D;&#x304F;&#x3079;&#x304D;</pr></trd>,
    <trd>&#x7570;&#x5E38;&#x306A;  <pr>&#x3044;&#x3058;&#x3087;&#x3046;&#x306A;</pr></trd>,
    <trd>&#x3081;&#x305A;&#x3089;&#x3057;&#x3044;</trd>
  </trdgrp>
  <trd lng="nl">fenomenaal</trd>
  <trd lng="pl">fenomenalny</trd>
  <trd lng="ru">&c_f;&c_je;&c_n;&c_o;&c_m;&c_je;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>fenomen&aacute;lny</trd>,
    <trd>neoby&ccaron;ajn&yacute;</trd>,
    <trd>nev&scaron;edn&yacute;</trd>,
    <trd>vynikaj&uacute;ci</trd>
  </trdgrp>
  <trd lng="tr">ola&gbreve;an&uuml;st&uuml;</trd>
  <trd lng="uk">&c_f;&c_je;&c_n;&c_o;&c_m;&c_je;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x4E0D;&#x5BFB;&#x5E38; <pr>b&#xF9;x&#xFA;nch&#xE1;ng</pr></trd>,
    <trd>&#x4E0D;&#x7406;&#x6027; <pr>b&#xF9;l&#x1D0;x&#xEC;ng</pr></trd>
  </trdgrp>
</drv>
</art>
