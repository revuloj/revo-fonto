<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>fenomen</rad>/o
</kap>
<drv mrk="fenome.0o">
  <kap><tld/>o</kap>
  <snc mrk="fenome.0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Perceptebla per la sentumoj apero; natura okaza&jcirc;o:
      <ekz>astronomiaj, fizikaj, spiritaj <tld/>oj.</ekz>
    </dif>
    <trdgrp lng="de">
     <trd>Ph&auml;nomen</trd>,
     <trd>Erfahrungstatsache</trd>
    </trdgrp>
    <trd lng="fr">ph&eacute;nom&egrave;ne</trd>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malofta, eksterordinara, stranga afero:
      <ekz>renkonti vin estas vera <tld/>o<fnt>B</fnt></ekz>.
    </dif>
    <refgrp tip="vid">
      <ref cel="mirakl.0o">miraklo</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Ph&auml;nomen</trd>,
      <trd>seltenes <ind>Ereignis</ind></trd>,
      <trd>seltsame <ind>Sache</ind></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>ph&eacute;nom&egrave;ne</trd>,
      <trd>merveille</trd>,
      <trd>prodige</trd>
    </trdgrp> 
  </snc>
  <trdgrp lng="ru">
    <trd>&c_ja;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_f;&c_je;&c_n;&c_o;&c_m;&c_je;&c_n;</trd>
  </trdgrp>
</drv>
<drv mrk="fenome.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Malofta, stranga, eksterordinara:
      <ekz><tld/>a aventuro, floro, estulo.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="monstr.0a">monstra</ref>.
    </refgrp>
  </snc>
  <trd lng="fr">ph&eacute;nom&eacute;nal</trd>
  <trd lng="ru">&c_f;&c_je;&c_n;&c_o;&c_m;&c_je;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
