<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: beatric.xml,v 1.4 2023/09/24 15:23:10 afido Exp $">
    <kap>
      <rad>Beatric</rad>/o
      <fnt><bib>PIV2</bib></fnt>
    </kap>

<drv mrk="beatric.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">NOM</uzo>
  <snc>
    <dif>
      Virina <ref tip="lst" lst="voko:personaj_nomoj" cel="nom.persona0o.GEN">persona nomo</ref>
      originanta el la okcitana lingvo:
      <ekz>
        &scirc;i estis tiel feli&ccirc;a, vidante, ke <tld/>o konvene edzini&gcirc;is
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> sugestas al <tld/>o mortigi sian patron
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=255">Alfred Nobel</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_B;&c_e;&c_a;&c_t;&c_r;&c_y;&c_s;&c_a;</trd>,
      <trd>&c_B;&c_e;&c_a;&c_t;&c_r;&c_y;&c_ch;&c_e;</trd>
    </trdgrp>
    <trd lng="de">Beatrice</trd>
    <trd lng="en">Beatrice</trd>
    <trd lng="nl">Beatrijs</trd>
    <trd lng="ru">&c_B;&c_je;&c_a;&c_t;&c_r;&c_i;&c_s;&c_a;</trd>
  </snc>
  <trd lng="ja">&#x30D9;&#x30A2;&#x30C8;&#x30EA;&#x30FC;&#x30C1;&#x30A7;</trd>
</drv>

  </art>

