<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>anorak</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="anorak.0o">
  <kap><tld/>o</kap>
  <snc mrk="anorak.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Pluvimuna <ref tip="super" cel="vest.super0o">supervesto</ref> kun
      manikoj kaj kapu&ccirc;o, sen anta&ubreve;a fendo kaj ordinare vatita, la&ubreve; eskima
      modelo:
      <ekz>
        &scirc;i <klr>...</klr> malfermis la <tld/>on kaj prezentis al li sian
        mamon
        <fnt><aut>Peter Feuchten, Karl Gustafsson trad.</aut>, <vrk>mia
        juna&gcirc;o en grenlando</vrk>, <lok>norda prismo, 1956, numero 2a, p.
        82-85a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Anorak</trd>
  <trd lng="en">anorak</trd>
  <trd lng="fr">anorak</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_o;&c_r;&c_a;&c_k;</trd>,
    <trd>&c_k;&c_u;&c_r;&c_t;&c_k;&c_a; &c_s; &c_k;&c_a;&c_p;&c_ju;&c_sh;&c_o;&c_n;&c_o;&c_m;</trd>
  </trdgrp>
</drv>
</art>

