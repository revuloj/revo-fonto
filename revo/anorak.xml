<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: anorak.xml,v 1.12 2023/08/30 18:23:34 revo Exp $">
<kap>
  <rad>anorak</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="anorak.0o">
  <kap><tld/>o</kap>
  <snc mrk="anorak.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Pluvimuna <ref tip="super" cel="vest.super0o">supervesto</ref> kun
      manikoj kaj kapu&ccirc;o, sen anta&ubreve;a fendo kaj ordinare vatita, la&ubreve;
      eskima
      modelo:
      <ekz>
        &scirc;i <klr>...</klr> malfermis la <tld/>on kaj prezentis al li
        sian
        mamon
        <fnt><aut>Peter Feuchten, Karl Gustafsson trad.</aut>, <vrk>mia
        juna&gcirc;o en grenlando</vrk>, <lok>norda prismo, 1956, numero 2a,
        p.
        82-85a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_a;&c_n;&c_o;&c_r;&c_a;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>anorak</trd>,
    <trd>dlouh&#xE1; v&#x11B;trovka <klr>(sport.)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Anorak</trd>,
    <trd>Parka</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>anorak</trd>,
    <trd>parka</trd>
  </trdgrp>
  <trd lng="fr">anorak</trd>
  <trd lng="he">&#x5DE;&#x5E2;&#x5D9;&#x5DC; &#x5D2;&#x5E9;&#x5DD;</trd>
  <trd lng="ja">&#x30A2;&#x30CE;&#x30E9;&#x30C3;&#x30AF;</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_o;&c_r;&c_a;&c_k;</trd>,
    <trd>&c_k;&c_u;&c_r;&c_t;&c_k;&c_a; &c_s; &c_k;&c_a;&c_p;&c_ju;&c_sh;&c_o;&c_n;&c_o;&c_m;</trd>
  </trdgrp>
  <trd lng="sk">vetrovka</trd>
  <trdgrp lng="uk">
    <trd>&c_a;&#x43D;&#x43E;&c_r;&c_a;&#x43A;</trd>,
    <trd>&c_v;&c_ib;&c_t;&c_r;&#x43E;&c_z;&c_a;&c_h;&c_i;&c_s;&#x43D;&c_a;&#xA0;&#x43A;&c_u;&c_r;&c_t;&#x43A;&c_a; &c_z;
    &#x43A;&c_a;&#x43F;&#x44E;&c_sh;&#x43E;&#x43D;&#x43E;&#x43C;</trd>
  </trdgrp>
</drv>
</art>

