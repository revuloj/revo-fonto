<art mrk="$Id: kantik.xml,v 1.18 2022/05/01 08:09:23 revo Exp $">
<kap>
  <rad>kantik</rad>/o
</kap>
<drv mrk="kantik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <uzo tip="fak">REL</uzo>
  <snc>
    <dif>
      Pia edifa <ref tip="super" cel="kant.0o">kanto</ref>:
      <ekz>
        la pastro de mia vila&gcirc;o praktikas la le&gcirc;on de Kristo kaj sa&gcirc;e predikas, ke pufaj
        promesoj ne &scirc;topas malplenon, kaj pli ol <tld/>oj la fakto edifas, ke e&ccirc;
        herezulo &ccirc;e li ne soifas, sed trovas &ccirc;e l' pastro &ccirc;ielspecimenon
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_m;&c_n; <klr>(&c_c;&c_a;&c_r;&c_k;&c_o;&c_w;&c_n;&c_y;)</klr></trd>
  <trd lng="ca">c&agrave;ntic</trd>
  <trdgrp lng="cs">
    <trd>cantica</trd>,
    <trd>chvalozp&ecaron;v</trd>,
    <trd>kantikum</trd>
  </trdgrp>
  <trd lng="de">Loblied</trd>
  <trd lng="en">canticle</trd>
  <trd lng="es">c&aacute;ntico</trd>
  <trd lng="fr">cantique</trd>
  <trd lng="hu">egyh&aacute;zi <ind>&eacute;nek</ind></trd>
  <trd lng="it">cantico</trd>
  <trdgrp lng="nl">
    <trd>kerklied</trd>,
    <trd>loflied</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kantyk</trd>,
    <trd>kantyka</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_i;&c_m;&c_n; <klr>(&c_c;&c_je;&c_r;&c_k;&c_o;&c_v;&c_n;&c_y;&c_j;)</klr></trd>
  <trdgrp lng="sk">
    <trd>chr&aacute;mov&yacute; spev</trd>,
    <trd>chv&aacute;lospev</trd>
  </trdgrp>
  <trd lng="uk">&#x433;&#x456;&#x43C;&#x43D;</trd>
</drv>
</art>
