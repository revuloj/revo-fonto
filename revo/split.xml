<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>split</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="split.0o">
  <kap><tld/>o</kap>
  <snc mrk="split.0o.ligna">
    <dif>
      Maldika peco de fendita ligno:
      <ekz>
        dishaki lignan &scirc;tipon en <tld/>ojn<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Holzsplitter</trd>,
      <trd>Sprei&szlig;el</trd>
    </trdgrp>
    <trd lng="ru">&c_shch;&c_je;&c_p;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="split.0o.ajna">
    <dif>
      Io similanta <tld/>on; disfendita&jcirc;o a&ubreve; disrompa&jcirc;o:
      <ekz>
        la <ind><tld/>a</ind>j gotikaj literoj.
        <trd lng="hu">sz&aacute;lk&aacute;s</trd>
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Splitter</trd>,
      <trd>Bruchst&uuml;ck</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_s;&c_k;&c_o;&c_l;&c_o;&c_k;</trd>,
      <trd>&c_o;&c_b;&c_l;&c_o;&c_m;&c_o;&c_k;</trd>,
      <trd>&c_shch;&c_je;&c_p;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_s;&c_k;&c_o;&c_l;&c_a;&c_k;</trd>, 
    <trd>&c_a;&c_b;&c_l;&c_o;&c_m;&c_a;&c_k;</trd>, 
    <trd>&c_t;&c_r;&c_e;&c_s;&c_k;&c_a;</trd>, 
    <trd>&c_s;&c_t;&c_r;&c_e;&c_m;&c_k;&c_a;</trd>, 
    <trd>&c_sh;&c_ch;&c_e;&c_p;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_t;&c_r;&c_je;&c_s;&c_ch;&c_i;&c_c;&c_a;</trd>, 
    <trd>&c_t;&c_r;&c_je;&c_s;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="en">splinter</trd>
  <trdgrp lng="fr">
    <trd>&eacute;charde</trd>,
    <trd>&eacute;clat<klr> (fragment)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szil&aacute;nk</trd>,
    <trd>sz&aacute;lka</trd>
    <trd>forg&aacute;cs</trd>,
    <trd>t&ouml;red&eacute;k</trd>
  </trdgrp>
  <trd lng="nl">splinter</trd>
  <trdgrp lng="sk">
    <trd>trieska</trd>,
    <trd>úlomok</trd>
  </trdgrp>
</drv>

<drv mrk="split.0i">
  <kap><tld/>i, <var><kap><tld/>igi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="split.0i.KOMUNE">
    <dif>
      Disfendi a&ubreve; disrompi en multajn pecojn:
      <ekz>
        atomoj <ind>dis<tld/>ita</ind>j per fortegaj elektraj
        elradia&jcirc;oj;
        <trd lng="ru">&c_r;&c_a;&c_s;&c_shch;&c_je;&c_p;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
      </ekz>
      <ekz>
        dis<tld/>ita E&ubreve;ropo ne kapablas konkurenci Amerikon;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        dis<tld/>igi siajn fortojn.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_ch;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>, 
    <trd>&c_r;&c_a;&c_s;&c_k;&c_a;&c_l;&c_o;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>zerst&uuml;ckeln</trd>,
    <trd>zerspellen</trd>,
    <trd>zertr&uuml;mmern</trd>,
    <trd>zerschmettern</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>diviser<klr> (en &eacute;clats, en fragments)</klr></trd>,
    <trd>fragmenter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>has&iacute;t</trd>,
    <trd>forg&aacute;csol</trd>
  </trdgrp>
  <trd lng="nl">versplinteren</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_shch;&c_je;&c_p;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_shch;&c_je;&c_p;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_a;&c_l;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_l;&c_o;&c_t;&c_mol;</trd>    
  </trdgrp>
  <trd lng="sk">odštiepiť</trd>
</drv>

<drv mrk="split.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="split.0igxi.KOMUNE">
    <dif>
      Disfendi&gcirc;i a&ubreve; disrompi&gcirc;i en multajn pecojn:
      <ekz>
        grandaj ondoj dis<tld/>i&gcirc;is kontra&ubreve; la rokoj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la lingvo for<tld/>i&gcirc;is en dialektojn;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        dis<tld/>i&gcirc;o de l' laboristaro en diversajn partiojn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fragme.0o">fragmento</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_ch;&c_a;&c_p;&c_ib;&c_c;&c_c;&c_a;</trd>, 
    <trd>&c_r;&c_a;&c_s;&c_k;&c_a;&c_l;&c_o;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>zersplittern</trd>,
    <trd>in <ind>St&uuml;cke</ind> gehen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>diviser</ind></trd>,
    <trd>se <ind>fractionner</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hasad</trd>,
    <trd>forg&aacute;csol&oacute;dik</trd>
  </trdgrp>
  <trd lng="nl">uiteenvallen</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_shch;&c_je;&c_p;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_l;&c_o;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
