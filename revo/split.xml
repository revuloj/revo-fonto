<art mrk="$Id: split.xml,v 1.25 2026/01/01 22:00:22 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>split</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="split.0o">
  <kap><tld/>o</kap>
  <snc mrk="split.0o.ligna">
    <dif>
      Maldika <ref tip="super" cel="pec.0o.rompajxo">peco</ref> de
      fendita <ref tip="malprt" cel="lign.0o">ligno</ref>:
      <ekz>
        la diablo enblovis al li <tld/>on en la okulon
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Valter</nom> la Mizerulo tiel fortege ekpu&scirc;is el la selo la Ursrajdanton, ke <tld/>oj
        de la lanco flugis en la aeron
        <fnt>
          <bib>RBa</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi devis helpi vendi <klr>[&#8230;]</klr> unu <tld/>on post la alia de la kruco, sur
        kiu nia Sinjoro kaj Savanto Jesuo Kristo elspiris sian lastan surteran
        suspiron
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
    </dif>
    <refgrp tip="sub">
       <ref cel="ken.0o.lumigilo">keno<sncref/></ref>,
       <ref cel="rabot.0ajxo">rabota&jcirc;o</ref>
    </refgrp>
    <trd lng="ca">estella</trd>
    <trdgrp lng="de">
      <trd>Holzsplitter</trd>,
      <trd>Sprei&szlig;el</trd>
    </trdgrp>
    <trd lng="ru">&c_shch;&c_je;&c_p;&c_k;&c_a;</trd>
    <trd lng="zh">&#26408;&#23633; <pr>m&ugrave;xi&egrave;</pr></trd>
</snc>
  <snc mrk="split.0o.ajna">
    <dif>
      Io similanta <tld/>on; disfendita&jcirc;o a&ubreve;
      disrompa&jcirc;o:
      <ekz>
        la rusa grenado <klr>[&#8230;]</klr> mortigis la kunludantojn per siaj <tld/>oj, lin
        la aerpremo &jcirc;etis dudek metrojn
        <fnt>
          <bib>LGA</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la frakasitaj vitro<tld/>oj <klr>(vd <ref cel="vitr.0o">vitrero</ref>)</klr>
        de &scirc;ia mondpercepto vundis &scirc;in nove kaj nove
        <fnt>
          <bib>KKr</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>tros</trd>,
      <trd>boc&iacute;</trd>,
      <trd>mica</trd>,
      <trd>metralla</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Splitter</trd>,
      <trd>Bruchst&uuml;ck</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_s;&c_k;&c_o;&c_l;&c_o;&c_k;</trd>,
      <trd>&c_o;&c_b;&c_l;&c_o;&c_m;&c_o;&c_k;</trd>,
      <trd>&c_shch;&c_je;&c_p;&c_k;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#30862;&#29255; <pr>su&igrave;pi&agrave;n</pr></trd>,
    <trd>&#24494;&#23569; <pr>w&#275;ish&#462;o</pr></trd>,
    <trd>&#30751; <pr>zh&#462;</pr></trd>,
    <trd>&#32048;&#34180;&#29255; <pr>x&igrave;b&oacute;pi&agrave;n</pr></trd>,
    <trd>&#32454;&#34180;&#29255; <pr>x&igrave;b&oacute;pi&agrave;n</pr></trd>,
    <trd>&#31232;&#24052;&#28866; <pr>x&#299;b&#257;l&agrave;n</pr></trd>,
    <trd>&#31232;&#24052;&#29211; <pr>x&#299;b&#257;l&agrave;n</pr></trd>,
    <trd>&#30862;&#22359; <pr>su&igrave;ku&agrave;i</pr></trd>,
    <trd>&#30862;&#22602; <pr>su&igrave;ku&agrave;i</pr></trd>,
    <trd>&#34368; <pr>j&#299;</pr></trd>,
    <trd>&#40783; <pr>j&#299;</pr></trd>,
    <trd>&#40785; <pr>j&#299;</pr></trd>,
    <trd>&#30900;&#20799; <pr>ch&aacute;r</pr></trd>,
    <trd>&#30900;&#20818; <pr>ch&aacute;r</pr></trd>
  </trdgrp>
</snc>
  <refgrp tip="vid">
     <ref cel="fragme.0o">fragmento</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_a;&c_s;&c_k;&c_o;&c_l;&c_a;&c_k;</trd>,
    <trd>&c_a;&c_b;&c_l;&c_o;&c_m;&c_a;&c_k;</trd>,
    <trd>&c_t;&c_r;&c_e;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_e;&c_m;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_ch;&c_e;&c_p;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_t;&c_r;&c_je;&c_s;&c_ch;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_s;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>st&rcaron;ep</trd>,
    <trd>t&rcaron;&iacute;ska</trd>,
    <trd>&uacute;lomek</trd>,
    <trd>&scaron;t&ecaron;pina</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>splinter</trd>,
    <trd>chip</trd>,
    <trd>sliver</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;charde</trd>,
    <trd>&eacute;clat<klr> (fragment)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szil&aacute;nk</trd>,
    <trd>sz&aacute;lka</trd>
    <trd>forg&aacute;cs</trd>,
    <trd>t&ouml;red&eacute;k</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#34180;&#29255;  <pr>&#12399;&#12367;&#12408;&#12435;</pr></trd>,
    <trd>&#26408;&#29255;  <pr>&#12371;&#12387;&#12401;</pr></trd>,
    <trd>&#30772;&#29255;  <pr>&#12399;&#12408;&#12435;</pr></trd>,
    <trd>&#12481;&#12483;&#12503;</trd>
  </trdgrp>
  <trd lng="nl">splinter</trd>
  <trdgrp lng="pl">
    <trd>drzazga</trd>,
    <trd>odprysk</trd>,
    <trd>scalak</trd>,
    <trd>trzaska </trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>trieska</trd>,
    <trd>&uacute;lomok</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_o;&c_s;&c_k;&c_o;&c_l;&c_o;&c_k;</trd>,
    <trd>&c_u;&c_l;&c_a;&c_m;&c_o;&c_k;</trd>,
    <trd>&c_t;&c_r;&c_ib;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_l;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="split.0i">
  <kap><tld/>i, <var><kap><tld/>igi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="split.0i.KOMUNE">
    <dif>
      Dis<ref tip="super" cel="fend.0i">fendi</ref> a&ubreve; disrompi en multajn pecojn:
      <ekz>
        iu intence dis<tld/>is la jam fajrotorditan veturilon por malhelpi fakulajn
        konkludojn
        <fnt>
          <bib>ChB</bib>
        </fnt>;
      </ekz>
      <ekz>
        ia objekto <klr>[&#8230;]</klr> trafis rompe kaj <tld/>ige kun mu&gcirc;anta bruo la boaton
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la eventoj, kiuj en tradicia romano prezenti&gcirc;as por la leganto sinsekve, &ccirc;e
        <nom>Llosa</nom> estas <tld/>itaj tiel, ke la pasinto, la nuno kaj la estonto
        kunekzistas kvaza&ubreve; samtempe
        <fnt>
          <bib>KFl</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dis.0igi">disigi</ref>,
      <ref cel="er.0igi">erigi</ref>,
      <ref cel="fend.0i.KOMUNE">fendi</ref>,
      <ref cel="frakci.0igi">frakciigi</ref>,
      <ref cel="fragme.0i">fragmenti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_ch;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_a;&c_l;&c_o;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>fendre</trd>,
    <trd>esberlar</trd>,
    <trd>badar</trd>,
    <trd>dividir</trd>,
    <trd>fragmentar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>od&scaron;t&ecaron;pit</trd>,
    <trd>t&rcaron;&iacute;&scaron;tit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>zerst&uuml;ckeln</trd>,
    <trd>zerspellen</trd>,
    <trd>zertr&uuml;mmern</trd>,
    <trd>zerschmettern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>sliver</trd>,
    <trd>splinter</trd>,
    <trd>split</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>diviser<klr> (en &eacute;clats, en fragments)</klr></trd>,
    <trd>fragmenter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>has&iacute;t</trd>,
    <trd>forg&aacute;csol</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12408;&#12368;</trd>,
    <trd>&#35010;&#12367;  <pr>&#12373;&#12367;</pr></trd>
  </trdgrp>
  <trd lng="nl">versplinteren</trd>
  <trdgrp lng="pl">
    <trd>&lstroke;upa&cacute;</trd>,
    <trd>roz&lstroke;upa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_shch;&c_je;&c_p;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_shch;&c_je;&c_p;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_a;&c_l;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_l;&c_o;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">od&scaron;tiepi&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_z;&c_k;&c_o;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_shch;&c_je;&c_p;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_s;&c_ib;&c_k;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#30862; <pr>su&igrave;</pr></trd>,
    <trd>&#25758;&#27584; <pr>zhu&agrave;nghu&#464;</pr></trd>,
    <trd>&#25758;&#27585; <pr>zhu&agrave;nghu&#464;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="split.0igxi">
  <kap><tld/>i&gcirc;i, <var><kap>sin <tld/>i</kap></var></kap>
  <snc mrk="split.0igxi.KOMUNE">
    <dif>
      Disfendi&gcirc;i a&ubreve; disrompi&gcirc;i en multajn pecojn:
      <ekz>
        la ligno facile <tld/>i&gcirc;is a&ubreve; rompi&gcirc;is, kiam ili volis kunfiksi la diversajn
        partojn
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>;
      </ekz>
      <ekz>
        la arb&scirc;elo estis tiel krude frostinta, ke &gcirc;i <tld/>i&gcirc;is
        <fnt>
          <bib>VRA</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni povus kredi la tutan &scirc;ipon dis<tld/>i&gcirc;anta
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        de supre <klr>[&#8230;]</klr> enfaladis kolono da hela lumo, <tld/>anta sin en mil
        fajrerojn sur fontano
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la provon reunuecigi la francan Movadon <klr>[&#8230;]</klr> sekvis plena dis<tld/>i&gcirc;o
        pro personaj &jcirc;aluzoj
        <fnt>
          <bib>EeP</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="dis.0igxi">disi&gcirc;i</ref>,
       <ref cel="fal.dis0i">disfali</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_ch;&c_a;&c_p;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_a;&c_l;&c_o;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>fendre&#8217;s</trd>,
    <trd>esberlar-se</trd>,
    <trd>badar-se</trd>,
    <trd>dividir-se</trd>,
    <trd>fragmentar-se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>zersplittern</trd>,
    <trd>in <ind>St&uuml;cke</ind> gehen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>splinter</trd>,
    <trd>sliver</trd>,
    <trd>split</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>diviser</ind></trd>,
    <trd>se <ind>fractionner</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hasad</trd>,
    <trd>forg&aacute;csol&oacute;dik</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12408;&#12370;&#12427;</trd>,
    <trd>&#35010;&#12369;&#12427;  <pr>&#12373;&#12369;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">uiteenvallen</trd>
  <trd lng="pl">roz&lstroke;upa&cacute; si&eogonek;</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_shch;&c_je;&c_p;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_l;&c_o;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_z;&c_k;&c_o;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_shch;&c_je;&c_p;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
