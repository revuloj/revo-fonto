<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>granit</rad>/o
</kap>
<drv mrk="granit.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Tre malmola erupcia roka&jcirc;o konsistanta el miksa&jcirc;o da glimo,
      kvarco kaj feldspato. 
    </dif>
  </snc>
  <trd lng="de">Granit</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_n;&c_i;&c_t;</trd>
  <trd lng="sv">granit</trd>
</drv>
<drv mrk="granit.0a">
  <kap><tld/>a</kap>
  <snc num="1">
    <dif>
      El <tld/>o: <ekz><tld/>aj pavimeroj, muroj.</ekz>
    </dif>
    <trd lng="ru">&c_g;&c_r;&c_a;&c_n;&c_i;&c_t;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Obstinega: <ekz>lia <tld/>a kapo ne enlasas alian opinion.</ekz>
    </dif>
    <trd lng="ru">&c_t;&c_v;&c_je;&c_r;&c_d;&c_o;&c_k;&c_a;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </snc>
</drv>
</art>
