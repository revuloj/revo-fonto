<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gladio.xml,v 1.22 2023/11/11 11:36:35 revo Exp $">
<kap>
  <rad>gladiol</rad>/o
</kap>
<drv mrk="gladio.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="iridac.0oj">iridacoj</ref>
      (<trd lng="la">Gladiolus</trd>), kun grandaj diverskoloraj floroj,
      kulturataj &gcirc;ardene:
      <ekz>
        personoj kiuj atingas la finlinion ricevas florojn de la familianoj kaj
        spektantoj, plejofte ili ricevas <tld/>ojn, pro tio oni nomas la alvenstraton
        &leftquot;<nac>Via Gladiola</nac>&rightquot; (Gladiola Vojo)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=200791">Internacia kvartaga mar&scirc;ado de Nimego</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/b/b5/Gladiolus_byzantinus_-_Yvoire_(1).jpg/383px-Gladiolus_byzantinus_-_Yvoire_(1).jpg" prm="CC BY-SA 3.0">floraro de Bizanca <tld/>o (Gladiolus byzantinus)
        <fnt>
          <aut>Jean-Pol Grandmont, propra verko</aut>
          <lok><url ref="&WCW;/File:Gladiolus_byzantinus_-_Yvoire_(1).jpg">&WCW;/File:Gladiolus_byzantinus_-_Yvoire_(1).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_g;&c_l;&c_ja;&c_d;&c_y;&c_jo;&c_l;&c_u;&c_s;</trd>
  <trd lng="bg">&c_g;&c_l;&c_a;&c_d;&c_i;&c_o;&c_l;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>gladiol</trd>,
    <trd>lliri</trd>,
    <trd>espadella</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>gladiol</trd>,
    <trd>gladiola</trd>,
    <trd>me&ccaron;&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gladiole</trd>,
    <trd>Siegwurz</trd>
  </trdgrp>
  <trd lng="es">gladiola</trd>
  <trd lng="fr">gla&iuml;eul</trd>
  <trdgrp lng="hu">
    <trd>kardvir&aacute;g</trd>,
    <trd>gladi&oacute;lusz</trd>,
    <trd>d&aacute;koska</trd>
  </trdgrp>
  <trd lng="ja">&#x30B0;&#x30E9;&#x30B8;&#x30AA;&#x30E9;&#x30B9;</trd>
  <trd lng="mk">&c_g;&c_l;&c_a;&c_d;&c_i;&c_o;&c_l;&c_a;</trd>
  <trd lng="nl">gladiool</trd>
  <trdgrp lng="pt">
   <trd>glad&iacute;olo</trd>, 
   <trd>espadana</trd>, 
   <trd>palma-de-santa-rita</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_l;&c_a;&c_d;&c_i;&c_o;&c_l;&c_u;&c_s;</trd>,
    <trd>&c_sh;&c_p;&c_a;&c_zh;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">gladiola</trd>
  <trd lng="uk">&c_g;&c_l;&c_a;&c_d;&c_ib;&c_o;&c_l;&c_u;&c_s;</trd>
  <trdgrp lng="zh">
    <trd>&#x5510;&#x83D6;&#x84B2; <pr>t&#xE1;ngch&#x101;ngp&#xFA;</pr></trd>,
    <trd>&#x5251;&#x5170; <pr>ji&#xE0;nl&#xE1;n</pr></trd>
  </trdgrp>
</drv>
</art>

