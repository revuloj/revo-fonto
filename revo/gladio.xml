<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gladiol</rad>/o
</kap>
<drv mrk="gladio.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="iridac.0oj">iridacoj</ref>
      (<trd lng="la">Gladiolus</trd>), kun grandaj diverskoloraj floroj,
      kulturataj &gcirc;ardene.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Gladiole</trd>,
    <trd>Siegwurz</trd>
  </trdgrp>
  <trd lng="fr">gla&iuml;eul</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_l;&c_a;&c_d;&c_i;&c_o;&c_l;&c_u;&c_s;</trd>,
    <trd>&c_sh;&c_p;&c_a;&c_zh;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>

