<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fosili.xml,v 1.23 2023/11/10 06:08:00 revo Exp $">
<kap>
  <rad>fosili</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fosili.0o">
  <kap><tld/>o</kap>
  <snc mrk="fosili.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      &Scirc;toni&gcirc;inta resta&jcirc;o a&ubreve; postsigno de prahistoria organismo, konservita en &scirc;tona&jcirc;o:
      <ekz>
        en la sekvanta mioceno la antropoidaj simioj estas jam tre disvasti&gcirc;inta,j iliaj
        fosilioj estas trovitaj en E&ubreve;ropo, Azio, Norda kaj Orienta Afriko
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de la mondvaste konata &leftquot;pra-birdo&rightquot; arkeopterigo trovi&gcirc;as en la Londona
        Naturhistoria Muzeo
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2008/009438.php">Fosilioj en suda Germanio</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la ekzistanta man&ccirc;ura estas rigardata de la fakuloj kiel <klr>[&#x2026;]</klr> vivanta
        <tld/>o
        <fnt>
          <aut>Sabeo Fu/Yamasaki Seik&ocirc;</aut>
            <vrk><url ref="&Monato;/2001/007025.php">Formortanta lingvo de malplimulto en
            &Ccirc;inio</url></vrk>
            <bib>Monato</bib>
            <lok>2001/01, p. 22</lok>
          </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="artefakt.0o">artefakto</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_k;&c_a;&c_p;&c_je;&c_n;&c_mol;</trd>,
    <trd>&c_a;&c_k;&c_a;&c_m;&c_ja;&c_n;&c_je;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>fos&iacute;lie</trd>,
    <trd>zkamen&ecaron;lina</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Fossil</trd>,
    <trd>Petrefakt</trd>,
    <trd>Versteinerung</trd>
  </trdgrp>
  <trd lng="en">fossil</trd>
  <trd lng="es">f&oacute;sil</trd>
  <trd lng="fr">fossile<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>k&ouml;v&uuml;let</trd>,
    <trd>fossz&iacute;lia</trd>
  </trdgrp>
  <trd lng="ja">&#x5316;&#x77F3;  <pr>&#x304B;&#x305B;&#x304D;</pr></trd>
  <trd lng="nl">fossiel<klr tip="ind"> (z.nw.)</klr></trd>
  <trd lng="pt">f&oacute;ssil</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_k;&c_o;&c_p;&c_a;&c_je;&c_m;&c_o;&c_je;</trd>,
    <trd>&c_o;&c_k;&c_a;&c_m;&c_je;&c_n;&c_je;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">skamenelina</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_k;&c_o;&c_p;&c_n;&c_a; &c_r;&c_o;&c_s;&c_l;&c_i;&c_n;&c_a;/&c_t;&c_v;&c_a;&c_r;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_m;&#x2019;&c_ja;&c_n;&c_ib;&c_l;&c_ib;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="zh">&#x5316;&#x77F3; <pr>hu&#xE0;sh&#xED;</pr></trd>
</drv>

<drv mrk="fosili.0a">
  <kap><tld/>a</kap>
  <snc mrk="fosili.0a.MIN">
    <dif>
      &Scirc;toneca, havanta econ de <tld/>o,
      rilatanta al <tld/>o:
      <ekz>
        <tld/>a rano kun konservi&gcirc;inta ha&ubreve;to el la mioceno
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2008/009438.php">Fosilioj en suda Germanio</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj piedsignoj kaj spuroj kaj birdovoj el la kratero
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2008/009438.php">Fosilioj en suda Germanio</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Michael Reynolds</nom> disvolvis konstrustilon, kiu perfekte kongruas kun la celoj de
        la protokolo de Kioto por malpliigi la forcejan efikon pro <tld/>a bruligo
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo DURWAEL</aut>
          <vrk><url ref="&Monato;/2004/008236.html">La rikolto de la nova tempo</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_k;&c_a;&c_p;&c_n;&c_jo;&c_v;&c_y;</trd>,
    <trd>&c_a;&c_k;&c_a;&c_m;&c_ja;&c_n;&c_je;&c_l;&c_y;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_m;&c_ja;&c_n;&c_je;&c_l;&c_y;</trd>,
    <trd>&c_v;&c_y;&c_m;&c_je;&c_r;&c_l;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>fossil</trd>,
    <trd>versteinert</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fossile<klr tip="ind"> (adj.)</klr></trd>,
    <trd>fossilis&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megk&ouml;vesedett</trd>,
    <trd>fosszilis</trd>,
    <trd>&aacute;satag</trd>
  </trdgrp>
  <trd lng="ja">&#x5316;&#x77F3;&#x306E;  <pr>&#x304B;&#x305B;&#x304D;&#x306E;</pr></trd>
  <trd lng="nl">fossiel<klr tip="ind"> (b.nw.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_k;&c_o;&c_p;&c_a;&c_je;&c_m;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_k;&c_a;&c_m;&c_je;&c_n;&c_je;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_y;&c_m;&c_je;&c_r;&c_sh;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_k;&c_o;&c_p;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_m;&#x2019;&c_ja;&c_n;&c_ib;&c_l;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="fosili.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="fosili.0igxi.MIN">
    <dif>
      <ref tip="vid" cel="sxton.0igxi.MIN">&Scirc;toni&gcirc;i</ref>,
      fari&gcirc;i <tld/>o:
      <ekz>
        grandaj <tld/>i&gcirc;intaj arboj <klr>[&#x2026;]</klr>
        ankora&ubreve; staras rektaj kiel ili kreskis
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 9a</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la unua pa&scirc;o en la direkto de reformoj devas esti la forigo de &ccirc;i tiaj
        <klr>[&#x2026;]</klr> <tld/>i&gcirc;intaj, priridetindaj tezoj
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#x2116;11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_c;&c_mol; <ind>&c_v;&c_y;&c_k;&c_a;&c_p;&c_n;&c_je;&c_m;</ind></trd>
  <trd lng="de">versteinern</trd>
  <trd lng="en">fossilize</trd>
  <trd lng="es">fosilizarse</trd>
  <trd lng="fr">se <ind>fossiliser</ind></trd>
  <trdgrp lng="hu">
    <trd>megk&ouml;vesedik</trd>,
    <trd>megk&ouml;v&uuml;l</trd>
  </trdgrp>
  <trd lng="ja">&#x5316;&#x77F3;&#x5316;&#x3059;&#x308B;  <pr>&#x304B;&#x305B;&#x304D;&#x304B;&#x3059;&#x308B;</pr></trd>
  <trdgrp lng="nl">
    <trd>verstenen</trd>,
    <trd>fossiliseren</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_t;&c_mol; &c_i;&c_s;&c_k;&c_o;&c_p;&c_a;&c_je;&c_m;&c_y;&c_m;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_k;&c_a;&c_m;&#x2019;&c_ja;&c_n;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_t;&c_v;&c_o;&c_r;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja; &c_n;&c_a; &c_s;&c_k;&c_a;&c_m;&#x2019;&c_ja;&c_n;&c_ib;&c_l;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_f;&c_o;&c_s;&c_i;&c_l;&c_ib;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
