<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>fosili</rad>/o</kap>
<drv mrk="fosili.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Nomo donata al &scirc;toni&gcirc;intaj vegeta&jcirc;oj a&ubreve;
animalaj resta&jcirc;oj
      a&ubreve; postsignoj, kiujn oni trovas en teraj tavoloj pli fruaj
      ol la nuna geologia epoko.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_k;&c_a;&c_p;&c_je;&c_n;&c_mol;</trd>,
    <trd>&c_a;&c_k;&c_a;&c_m;&c_ja;&c_n;&c_je;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Fossil</trd>,
    <trd>Petrefakt</trd>,
    <trd>Versteinerung</trd>
  </trdgrp>
  <trd lng="en">fossil</trd>
  <trd lng="es">f&oacute;sil</trd>
  <trd lng="fr">fossile<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>k&ouml;v&uuml;let</trd>,
    <trd>fossz&iacute;lia</trd>
  </trdgrp>
  <trd lng="nl">fossiel<klr tip="ind"> (z.nw.)</klr></trd>
  <trd lng="pt">f&#243;ssil</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_k;&c_o;&c_p;&c_a;&c_je;&c_m;&c_o;&c_je;</trd>,
    <trd>&c_o;&c_k;&c_a;&c_m;&c_je;&c_n;&c_je;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="fosili.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>o, havanta econ de <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_k;&c_a;&c_p;&c_n;&c_jo;&c_v;&c_y;</trd>,
    <trd>&c_a;&c_k;&c_a;&c_m;&c_ja;&c_n;&c_je;&c_l;&c_y;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_m;&c_ja;&c_n;&c_je;&c_l;&c_y;</trd>,
    <trd>&c_v;&c_y;&c_m;&c_je;&c_r;&c_l;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>fossil</trd>,
    <trd>versteinert</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fossile<klr tip="ind"> (adj.)</klr></trd>,
    <trd>fossilis&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megk&ouml;vesedett</trd>,
    <trd>fosszilis</trd>,
    <trd>&aacute;satag</trd>
  </trdgrp>
  <trd lng="nl">fossiel<klr tip="ind"> (b.nw.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_k;&c_o;&c_p;&c_a;&c_je;&c_m;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_k;&c_a;&c_m;&c_je;&c_n;&c_je;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_y;&c_m;&c_je;&c_r;&c_sh;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="fosili.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Fari&gcirc;i <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_c;&c_mol;
<ind>&c_v;&c_y;&c_k;&c_a;&c_p;&c_n;&c_je;&c_m;</ind></trd>
  <trd lng="de">versteinern</trd>
  <trd lng="en">fossilize</trd>
  <trd lng="es">fosilizarse</trd>
  <trd lng="fr">se <ind>fossiliser</ind></trd>
  <trdgrp lng="hu">
    <trd>megk&ouml;vesedik</trd>,
    <trd>megk&ouml;v&uuml;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verstenen</trd>,
    <trd>fossiliseren</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_t;&c_mol;
&c_i;&c_s;&c_k;&c_o;&c_p;&c_a;&c_je;&c_m;&c_y;&c_m;</trd>
</drv>
</art>
