<drv mrk="erar.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="erar.0ema.KOMUNE">
    <dif>
      Povanta <tld/>i:
      <ekz>
        la papo estas <ind>ne<tld/>ema</ind>
        kiam li decidas solene pri dogmo;
        <trd lng="de">unfehlbar</trd>
        <trd lng="fr">infaillible</trd>
        <trd lng="hu">t&eacute;vedhetetlen</trd>
      </ekz>
      <ekz>
        sed neniu homo estas ne<tld/>ema
        <fnt>
          <bib>Monato</bib>
          <aut>Petro Desmet'</aut>
          <vrk><url ref="&Monato;/2006/008993.php">La unua Zamenhof-monumento de la 21a jarcento:
          dika Pomego!</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        homoj estas <tld/>emaj &ndash; nur la katolika papo estas memdifine neeraripova
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2009/009914.php">Gazet-eraroj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        a&ubreve;toroj kaj redaktoroj estas <tld/>emaj homoj, do korektu iliajn <tld/>ojn kaj
        kritiku
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2006/008941.php">Skribu leterojn!</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_h;&c_ib;&c_l;&c_mol;&c_n;&c_y; &c_d;&c_a; &c_p;&c_a;&c_m;&c_y;&c_l;&c_a;&c_k;</trd>
