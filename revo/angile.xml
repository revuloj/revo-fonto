<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: angile.xml,v 1.10 2022/03/28 09:34:03 revo Exp $">
<kap>
  <rad>angiled</rad>/oj
</kap>
<drv mrk="angile.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> de <ref
      tip="malprt" cel="angil.0oformaj">angiloformaj</ref>
      fi&scirc;oj (<trd lng="la">Anguillidae</trd>), kun muka, skvama
      ha&ubreve;to:
      <ekz>
        <tld/>oj estas fi&scirc;oj migrantaj riverojn malsupren pasigante sian
        plenkreskan vivon
        en dol&ccirc;akvo sed frajantaj en maro.
      </ekz>
    </dif>
    <ref tip="prt" cel="angil.0o">angilo</ref>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_ch;&c_n;&c_y;&c_ja; &c_v;&c_u;&c_g;&c_r;&c_y; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  <trd lng="ca">angu&iacute;l&middot;lids</trd>
  <trd lng="de">Aale</trd>
  <trd lng="fr">Anguillid&eacute;s</trd>
  <trd lng="he">&#x5E6;&#x5DC;&#x5D5;&#x5E4;&#x5D7;&#x5D9;&#x5DD;</trd>
  <trd lng="hu">angolnaf&eacute;l&eacute;k</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_ch;&c_n;&c_y;&c_je; <ind>&c_u;&c_g;&c_r;&c_i;</ind>
<klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_p;&c_r;&c_je;&c_s;&c_n;&c_o;&c_v;&c_o;&c_d;&c_n;&c_y;&c_je;
<ind>&c_u;&c_g;&c_r;&c_i;</ind> <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
</drv>
</art>

