<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: teatr.xml,v 1.46 2022/05/05 17:08:11 revo Exp $">
<kap>
  <ofc>*</ofc><ofc>1</ofc><ofc>7</ofc>
  <rad>teatr</rad>/o
</kap>
<drv mrk="teatr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="teatr.0o.belarto">
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="lst" cel="art.bel0o" lst="voko:artoj">Belarto</ref>
      pri verkado kaj aran&gcirc;ado de rakontoj prezentotaj
      de aktoroj a&ubreve; pupoj al spektantoj:
      <ekz>
        <tld/>amanto ofte vizitas la <tld/>on kaj ricevas balda&ubreve;
        <tld/>ajn
        manierojn 
        <fnt><bib>F</bib>, <lok>&FE; 32</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i ne dancas, &#x2010; &scirc;i ne fumas, <klr>[&#x2026;]</klr> &scirc;i ne revas pri
        <tld/>o
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga
          Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la historio de e&ubreve;ropa <tld/>o
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato1/monato-9310-artikoletoj.txt">Malkopiilo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        de miaj infanaj jaroj mi amas la <tld/>on
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 11a, p.
        453a</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">POL</uzo>
        <uzo tip="stl">FIG</uzo>
        iuj instancoj estis ekscititaj, aliaj parolis pri somera <tld/>o
        <klr>(politika <ref tip="sub" cel="fars.0o">farso</ref> dum
        someraj ferioj)</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Maul Stefan</aut>
          <vrk><url ref="&Steloj;monato2/007149.html">Reform-fiasko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="es">arte dram&aacute;tico</trd>
    <trdgrp lng="fr">
      <trd>art dramatique</trd>
    </trdgrp>
    <trd lng="pl">teatr</trd>
    <trd lng="ro">teatru</trd>
  </snc>
  <snc mrk="teatr.0o.ejo">
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="dif" cel="teatr.0ejo"><tld lit="T"/>ejo</ref>:
      <ekz>
         oni kure kolekti&gcirc;is <klr>[&#x2026;]</klr> en la <tld/>on
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 19:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ekvivis tre gaje, iradis tre ofte en la <tld/>on
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        vespere okazis prezentado en fama amatora <tld/>o,
        <klr>[&#x2026;]</klr> &ccirc;iuj lokoj en la <tld/>o estis okupitaj
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        pro &scirc;ia bela vo&ccirc;o <klr>[&#x2026;]</klr> oni &scirc;in akceptis en la <tld/>on
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la koncernan petskribon alportis en la <tld/>on lia edzino
        <fnt><bib>MkM</bib>, <lok>epilogo, p. 338a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="es">teatro</trd>
    <trd lng="pl">teatr</trd>
    <trd lng="zh">&#x5267;&#x573A; [<ind>j&ugrave;ch&#x1CE;ng</ind>]</trd>
  </snc>
  <snc mrk="teatr.0o.spektantaro">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Spektantaro:
      <ekz>
        la malla&ubreve;do de <klr>[kompetenta]</klr> homo devas esti por vi
        pli
        grava ol la apla&ubreve;dado de tuta <tld/>o da aliaj personoj
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi devas vesti min por la <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="es">p&uacute;blico</trd>
  </snc>
  <snc mrk="teatr.0o.ajxo">
    <dif>
      <tld lit="T"/>a&jcirc;o:
      <ekz>
        ili tiel krias kontra&ubreve; la <tld/>oj de la popolamaso
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        la vivo tie estas delikata <klr>[&#x2026;]</klr>: <tld/>oj, hundoj por
        vi dancas, kaj &ccirc;io, kion vi volas
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>

<!-- dubinda senco, neniu ekz-o trovita
  <snc mrk="teatr.0o.aktoreco">
    <uzo tip="stl">RAR</uzo>
    <dif>
      Aktora profesio.
    </dif>
  </snc>
-->
  <trd lng="be">&c_t;&c_e;&c_a;&c_t;&c_a;&c_r;</trd>
  <trd lng="bo">&#xF5F;&#xFB3;&#xF7C;&#xF66;&#xF0B;&#xF42;&#xF62;&#xF0B;&#xF41;&#xF44;&#xF0B;</trd>
  <trd lng="cs">divadlo</trd>
  <trd lng="de">Theater</trd>
  <trd lng="en">theatre</trd>
  <trd lng="es">teatro</trd>
  <trdgrp lng="fr">
    <trd>th&eacute;&acirc;tre</trd>
  </trdgrp>
  <trd lng="hu">sz&iacute;nh&aacute;z</trd>
  <trd lng="id">teater</trd>
  <trdgrp lng="nl">
    <trd>theater</trd>,
    <trd>toneel</trd>
  </trdgrp>
  <trd lng="pl">teatr</trd>
  <trdgrp lng="pt">
   <trd>teatro</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_a;&c_t;&c_r;</trd>
  <trd lng="sk">divadlo</trd>
  <trd lng="sv">teater</trd>
  <trd lng="uk">&#x442;&#x435;&#x430;&#x442;&#x440;</trd>
</drv>

<drv mrk="teatr.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="teatr.0a.teatrajxo">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Rilata al la <tld/>o, la arto a&ubreve; la domo:
      <ekz>
        <tld/>a direktoro
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        dancistinoj <tld/>aj
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a prezentado
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;parmonujo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Theater-</trd>
  </snc>
  <snc mrk="teatr.0a.figure">
    <uzo tip="stl">FIG</uzo>
    <dif>
     Konvena al aktoro, anta&ubreve;kalkulita por fari efekton:
      <ekz>
        <tld/>aj manieroj
        <fnt><bib>F</bib>, <lok>&FE; 32</lok></fnt>;
      </ekz>
      <ekz>
        li kun <tld/>a gesto montris la pordon de la kabineto
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiun <tld/>an pozon akompanis viza&gcirc;esprimo, kiu 
        signifis anka&ubreve; <tld/>an kaj tre komike aspektantan ravitecon
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">theatralisch</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>th&eacute;&acirc;tral</trd>
    <trd>th&eacute;&acirc;trale</trd>
  </trdgrp>
  <trd lng="uk">&#x442;&#x435;&#x430;&#x442;&#x440;&#x430;&#x43B;&#x44C;&#x43D;&#x438;&#x439;</trd>
</drv>

<!-- sola ekz-o - &#331;i jam aperas sub ~o
<drv mrk="teatr.0amanto">
  <kap><ofc>*</ofc><ofc>ekz</ofc><tld/>amanto</kap>
  <snc>
    <dif>
     Homo, kiu ofte vizitas teatron kaj &scirc;atas teatra&jcirc;ojn:
      <ekz>
      Teatramanto ofte vizitas la teatron kaj ricevas balda&ubreve; teatrajn
manierojn <fnt><bib>F</bib>, <lok>&FE; 32</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>th&eacute;&acirc;tral</trd>
    <trd>th&eacute;&acirc;trale</trd>
  </trdgrp>
</drv>
-->

<drv mrk="teatr.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="teatr.0ajxo.TEA">
    <tezrad fak="TEA"/>	  
    <uzo tip="fak">TEA</uzo>
    <dif>
      <tld lit="T"/>a verko kun ties roloj, dialogoj kaj agadoj:
      <ekz>
        a&ubreve;skulti <tld/>a&jcirc;ojn
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Dua Kongreso Esperantista en Gen&egrave;ve en
          la 28a de a&ubreve;gusto 1906</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a&jcirc;o traktis pri du geamantoj, al kiuj la sorto ne
        permesis ricevi sin reciproke
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tistino kaj kamentubisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ja nur <tld/>a&jcirc;o, kiu devas nin amuzi
        <fnt>
          <bib>Fab3</bib>
          <lok>Bona humoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio estis mirinde bela <tld/>a&jcirc;o kaj tute ne malgaja, sed &gcirc;uste
        en la
        momento, kiam la re&gcirc;ino sin levis <klr>[&#x2026;]</klr> la hundo, ne
        tenata
        <klr>[&#x2026;]</klr> saltis en la <tld/>on, kaptis la re&gcirc;inon
        <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        muzik<tld/>a&jcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Thomas S&Uuml;LZLE</aut>
          <vrk><url ref="&Monato;/2003/007993.html">Komenco
          de la teatra maratono</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kroataj <tld/>a&jcirc;oj kaj aktoroj multe kontribuis al la
        esperanta <tld/>o
        <fnt>
          <aut>B. Kolker</aut>,
          <vrk>Teatra antologio</vrk>,
          <bib>Monato</bib>,
          <lok>1998:3, p. 18a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Theaterst&uuml;ck</trd>
  </snc>
  <trd lng="be">&c_p;'&c_je;&c_s;&c_a;</trd>
  <trdgrp lng="es">
    <trd>representaci&oacute;n</trd>,
    <trd>pieza de teatro</trd>,
    <trd>obra de teatro</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>pi&egrave;ce de th&eacute;&acirc;tre</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&iacute;nm&udblac;</trd>,
    <trd>sz&iacute;ndarab</trd>
  </trdgrp>
  <trd lng="nl">toneelstuk</trd>
  <trdgrp lng="pl">
    <trd>sztuka <ind>teatralna</ind></trd>,
    <trd>przedstawienie <ind>teatralne</ind></trd>,
    <trd>spektakl <ind>teatralny</ind></trd>,
  </trdgrp>
  <trd lng="ru">&c_p;&c_mol;&c_je;&c_s;&c_a;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x2019;&#x454;&#x441;&#x430;</trd>,
    <trd>&#x442;&#x435;&#x430;&#x442;&#x440;&#x430;&#x43B;&#x44C;&#x43D;&#x430; &#x43F;&#x43E;&#x441;&#x442;&#x430;&#x43D;&#x43E;&#x432;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>

<drv mrk="teatr.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="teatr.0ejo.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">TEA</uzo>
    <dif>
      Loko, ordinare <ref tip="super" cel="konstr.0ajxo">
      konstrua&jcirc;o</ref>,
      en kiu oni prezentas <tld/>a&jcirc;ojn:
      <ekz>
        la blindige brilaj lumtuboj de la <esc>strat'angula</esc>
        <tld/>ejo
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 21a, p. 204a</lok>
        </fnt>;
      </ekz>
      <ekz>
        plurfoje ekzistis itala <tld/>ejo en Parizo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=231729">Itala
          teatrejo de Parizo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="teatr.0o.ejo">teatro<sncref/></ref>
    <trd lng="de">Theater<klr>(haus)</klr></trd>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_a;&c_t;&c_a;&c_r;<klr>
  (&c_b;&c_u;&c_d;&c_y;&c_n;&c_a;&c_k;)</klr></trd>
  <trd lng="cs">divadlo <klr>(budova)</klr></trd>
  <trd lng="es">teatro</trd>
  <trdgrp lng="fr">
    <trd>th&eacute;&acirc;tre<klr> (b&acirc;timent)</klr></trd>
  </trdgrp>
  <trd lng="hu">sz&iacute;nh&aacute;z<klr tip="amb">
  (&eacute;p&uuml;let)</klr></trd>
  <trd lng="nl">schouwburg</trd>
  <trd lng="pl">teatr</trd>
  <trd lng="ru">&c_t;&c_je;&c_a;&c_t;&c_r;<klr tip="amb">
  (&c_z;&c_d;&c_a;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">divadlo <klr>(budova)</klr></trd>
  <trd lng="zh">&#x5267;&#x573A; [<ind>j&ugrave;ch&#x1CE;ng</ind>]</trd>
</drv>

