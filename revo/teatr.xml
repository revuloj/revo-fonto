<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>teatr</rad>/o
</kap>
<drv mrk="teatr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEA</uzo>
  <snc mrk="teatr.0o.belarto">
    <dif>
      <ref tip="super" cel="art.bel0o">Belarto</ref>
      pri verkado kaj aran&gcirc;ado de rakontoj prezentotaj
      de aktoroj a&ubreve; pupoj al spektantoj.
    </dif>
    <trdgrp lng="fr">
      <trd>art dramatique</trd>
    </trdgrp>
  </snc>
  <snc mrk="teatr.0o.spektantaro">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Spektantaro:
      <ekz>
        la malla&ubreve;do de kompetenta homo devas esti por vi pli grava ol
la
        apla&ubreve;doj de tuta <tld/>o da aliaj personoj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="teatr.0o.aktoreco">
    <dif>
      Aktora profesio.
    </dif>
  </snc>
  <snc mrk="teatr.0o.ejo">
    <ref tip="dif" cel="teatr.0ejo"><tld/>ejo</ref>
  </snc>
  <trd lng="de">Theater</trd>
  <trd lng="en">theatre</trd>
  <trdgrp lng="fr">
    <trd>th&eacute;&acirc;tre</trd>
  </trdgrp>
  <trd lng="hu">sz&iacute;nh&aacute;z</trd>
  <trd lng="nl">theater</trd>
  <trd lng="ru">&c_t;&c_je;&c_a;&c_t;&c_r;</trd>
</drv>

<drv mrk="teatr.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <dif>
      Teatra verko kun ties roloj, dialogoj kaj agadoj.
    </dif>
    <refgrp tip="sin">
      <ref cel="dram.0o.teatrajxo">dramo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>pi&egrave;ce de th&eacute;&acirc;tre</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&iacute;nm&udblac;</trd>,
    <trd>sz&iacute;ndarab</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_mol;&c_je;&c_s;&c_a;</trd>
</drv>

<drv mrk="teatr.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Konstrua&jcirc;o, en kiu prezentatas teatra&jcirc;oj.
    </dif>
    <refgrp tip="sin">
      <ref cel="teatr.0o.ejo">teatro</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>th&eacute;&acirc;tre<klr> (b&acirc;timent)</klr></trd>
  </trdgrp>
  <trd lng="hu">sz&iacute;nh&aacute;z <klr
tip="amb">(&eacute;p&uuml;let)</klr></trd>
  <trd lng="ru">&c_t;&c_je;&c_a;&c_t;&c_r; <klr
tip="amb">(&c_z;&c_d;&c_a;&c_n;&c_i;&c_je;)</klr></trd>
</drv>

<drv mrk="teatr.film0o">
  <kap>film<tld/>o</kap>
  <uzo tip="fak">KIN</uzo>
  <snc>
    <dif>
      Konstrua&jcirc;o, en kiu oni prezentas al la rigardantaro
projekciatajn
      bildojn.
    </dif>
    <refgrp tip="sin">
      <ref cel="kin.0ejo0oteatro">kinejo</ref>.
    </refgrp>
  </snc>
  <trd lng="hu">filmsz&iacute;nh&aacute;z</trd>
  <trd lng="nl">bioscoop</trd>
  <trd lng="ru">&c_k;&c_i;&c_n;&c_o;&c_t;&c_je;&c_a;&c_t;&c_r;</trd>
</drv>

<drv mrk="teatr.pup0o">
  <kap>pup<tld/>o</kap>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="teatr.0o.belarto">Teatro</ref>
      en kiu rolas pupoj.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>th&eacute;&acirc;tre de marionnettes</trd>
  </trdgrp>
  <trd lng="hu">b&aacute;bsz&iacute;nh&aacute;z</trd>
  <trd lng="ru">&c_k;&c_u;&c_k;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j;
<ind>&c_t;&c_je;&c_a;&c_t;&c_r;</ind></trd>
</drv>
</art>
