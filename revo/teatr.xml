<drv mrk="teatr.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="teatr.0ajxo.TEA">
    <tezrad fak="TEA"/>
    <uzo tip="fak">TEA</uzo>
    <dif>
      <tld lit="T"/>a verko kun ties roloj, dialogoj kaj agadoj:
      <ekz>
        a&ubreve;skulti <tld/>a&jcirc;ojn
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Dua Kongreso Esperantista en Gen&egrave;ve en
          la 28a de a&ubreve;gusto 1906</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a&jcirc;o traktis pri du geamantoj, al kiuj la sorto ne
        permesis ricevi sin reciproke
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tistino kaj kamentubisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ja nur <tld/>a&jcirc;o, kiu devas nin amuzi
        <fnt>
          <bib>Fab3</bib>
          <lok>Bona humoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio estis mirinde bela <tld/>a&jcirc;o kaj tute ne malgaja, sed &gcirc;uste
        en la
        momento, kiam la re&gcirc;ino sin levis <klr>[&#8230;]</klr> la hundo, ne
        tenata
        <klr>[&#8230;]</klr> saltis en la <tld/>on, kaptis la re&gcirc;inon
        <klr>[&#8230;]</klr>
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        muzik<tld/>a&jcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Thomas S&Uuml;LZLE</aut>
          <vrk><url ref="&Monato;/2003/007993.html">Komenco
          de la teatra maratono</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kroataj <tld/>a&jcirc;oj kaj aktoroj multe kontribuis al la
        esperanta <tld/>o
        <fnt>
          <aut>B. Kolker</aut>,
          <vrk>Teatra antologio</vrk>,
          <bib>Monato</bib>,
          <lok>1998:3, p. 18a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Theaterst&uuml;ck</trd>
  </snc>
  <trd lng="be">&c_p;'&c_je;&c_s;&c_a;</trd>
  <trdgrp lng="es">
    <trd>representaci&oacute;n</trd>,
    <trd>pieza de teatro</trd>,
    <trd>obra de teatro</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>pi&egrave;ce de th&eacute;&acirc;tre</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&iacute;nm&udblac;</trd>,
    <trd>sz&iacute;ndarab</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x622F;&#x66F2;  <pr>&#x304E;&#x304D;&#12423;&#x304F;</pr></trd>,
    <trd>&#21127;  <pr>&#12370;&#x304D;</pr></trd>,
    <trd>&#x51FA;&#12375;&#29289;  <pr>&#12384;&#12375;&#12418;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">toneelstuk</trd>
  <trdgrp lng="pl">
    <trd>sztuka <ind>teatralna</ind></trd>,
    <trd>przedstawienie <ind>teatralne</ind></trd>,
    <trd>spektakl <ind>teatralny</ind></trd>,
  </trdgrp>
  <trd lng="ru">&c_p;&c_mol;&c_je;&c_s;&c_a;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#8217;&c_jeu;&c_s;&c_a;</trd>,
    <trd>&c_t;&c_je;&c_a;&c_t;&c_r;&c_a;&#x43B;&#x44C;&#x43D;&c_a; &#x43F;&#x43E;&c_s;&c_t;&c_a;&#x43D;&#x43E;&c_v;&#x43A;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="teatr.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="teatr.0ejo.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">TEA</uzo>
    <dif>
      Loko, ordinare <ref tip="super" cel="konstr.0ajxo">
      konstrua&jcirc;o</ref>,
      en kiu oni prezentas <tld/>a&jcirc;ojn:
      <ekz>
        la blindige brilaj lumtuboj de la <esc>strat'angula</esc>
        <tld/>ejo
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 21a, p. 204a</lok>
        </fnt>;
      </ekz>
      <ekz>
        plurfoje ekzistis itala <tld/>ejo en Parizo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=231729">Itala
          teatrejo de Parizo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="teatr.0o.ejo">teatro<sncref/></ref>
    <trd lng="de">Theater<klr>(haus)</klr></trd>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_a;&c_t;&c_a;&c_r;<klr>
  (&c_b;&c_u;&c_d;&c_y;&c_n;&c_a;&c_k;)</klr></trd>
  <trd lng="cs">divadlo <klr>(budova)</klr></trd>
  <trd lng="es">teatro</trd>
  <trdgrp lng="fr">
    <trd>th&eacute;&acirc;tre<klr> (b&acirc;timent)</klr></trd>
  </trdgrp>
  <trd lng="hu">sz&iacute;nh&aacute;z<klr tip="amb">
  (&eacute;p&uuml;let)</klr></trd>
  <trd lng="nl">schouwburg</trd>
  <trd lng="pl">teatr</trd>
  <trd lng="ru">&c_t;&c_je;&c_a;&c_t;&c_r;<klr tip="amb">
  (&c_z;&c_d;&c_a;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">divadlo <klr>(budova)</klr></trd>
  <trd lng="zh">&#21095;&#x573A; <pr>j&ugrave;ch&#x1CE;ng</pr></trd>
</drv>

