<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>teatr</rad>/o</kap>
<drv mrk="teatr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEA</uzo>
  <snc mrk="teatr.0o.belarto">
    <dif>
      <ref tip="super" cel="art.bel0o">Belarto</ref>
      pri verkado kaj aran&gcirc;ado de rakontoj prezentotaj
      de aktoroj a&ubreve; pupoj al spektantoj:
      <ekz>
        de miaj infanaj jaroj mi amas la <tld/>on
        <fnt><bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 11a, p.
        453a</lok></fnt>
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>art dramatique</trd>
    </trdgrp>
  </snc>
  <snc mrk="teatr.0o.ejo">
    <ref tip="dif" cel="teatr.0ejo"><tld/>ejo</ref>
    <ekz>
      la koncernan petskribon alportis en la <tld/>on lia edzino
      <fnt><bib>MkM</bib>, <lok>epilogo, p. 338a</lok></fnt>;
    </ekz>
  </snc>
  <snc mrk="teatr.0o.spektantaro">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Spektantaro:
      <ekz>
        la malla&ubreve;do de kompetenta homo devas esti por vi pli grava ol
        la apla&ubreve;doj de tuta <tld/>o da aliaj personoj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="teatr.0o.aktoreco">
    <uzo tip="stl">RAR</uzo>
    <dif>
      Aktora profesio.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_a;&c_t;&c_a;&c_r;</trd>
  <trd lng="de">Theater</trd>
  <trd lng="en">theatre</trd>
  <trdgrp lng="fr">
    <trd>th&eacute;&acirc;tre</trd>
  </trdgrp>
  <trd lng="hu">sz&iacute;nh&aacute;z</trd>
  <trdgrp lng="nl">
    <trd>theater</trd>,
    <trd>toneel</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_a;&c_t;&c_r;</trd>
</drv>

<drv mrk="teatr.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <dif>
      <tld lit="T"/>a verko kun ties roloj, dialogoj kaj agadoj:
      <ekz>
        kroataj <tld/>a&jcirc;oj kaj aktoroj multe kontribuis al la esperanta
        <tld/>o
        <fnt><aut>Boris Kolker</aut>: <vrk>Teatra antologio (Hecimovic
        Branko, Antologio de kroataj unuaktaj dramoj)</vrk>,
        <bib>Monato</bib>, <lok>jaro 1998a, numero 3a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="dram.0o.teatrajxo">dramo <sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;'&c_je;&c_s;&c_a;</trd>
  <trdgrp lng="fr">
    <trd>pi&egrave;ce de th&eacute;&acirc;tre</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&iacute;nm&udblac;</trd>,
    <trd>sz&iacute;ndarab</trd>
  </trdgrp>
  <trd lng="nl">toneelstuk</trd>
  <trd lng="ru">&c_p;&c_mol;&c_je;&c_s;&c_a;</trd>
</drv>

<drv mrk="teatr.0ejo">
  <kap><tld/>ejo</kap>
  <uzo tip="fak">ARKI</uzo>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <dif>
      Loko, ordinare <ref tip="super" cel="konstr.0ajxo">konstrua&jcirc;o</ref>,
      en kiu oni prezentas <tld/>a&jcirc;ojn:
      <ekz>
        la blindige brilaj lumtuboj de la strat'angula <tld/>ejo
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 21a, p. 204a</lok></fnt>;
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="teatr.0o.ejo">teatro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_a;&c_t;&c_a;&c_r;<klr> (&c_b;&c_u;&c_d;&c_y;&c_n;&c_a;&c_k;)</klr></trd>
  <trdgrp lng="fr">
    <trd>th&eacute;&acirc;tre<klr> (b&acirc;timent)</klr></trd>
  </trdgrp>
  <trd lng="hu">sz&iacute;nh&aacute;z<klr tip="amb"> (&eacute;p&uuml;let)</klr></trd>
  <trd lng="nl">schouwburg</trd>
  <trd lng="ru">&c_t;&c_je;&c_a;&c_t;&c_r;<klr tip="amb"> (&c_z;&c_d;&c_a;&c_n;&c_i;&c_je;)</klr></trd>
</drv>

<drv mrk="teatr.film0o">
  <kap>film<tld/>o</kap>
  <uzo tip="fak">KIN</uzo>
  <snc>
    <ref tip="dif" cel="kin.0ejo">kinejo</ref>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_n;&c_a;&c_t;&c_e;&c_a;&c_t;&c_a;&c_r;</trd>
  <trd lng="fr">cin&eacute;ma<klr> (lieu)</klr></trd>
  <trd lng="hu">filmsz&iacute;nh&aacute;z</trd>
  <trd lng="nl">bioscoop</trd>
  <trd lng="ru">&c_k;&c_i;&c_n;&c_o;&c_t;&c_je;&c_a;&c_t;&c_r;</trd>
</drv>

<drv mrk="teatr.kino0o">
  <kap>kino<tld/>o</kap>
  <uzo tip="fak">KIN</uzo>
  <snc>
    <ref tip="dif" cel="kin.0ejo">kinejo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_n;&c_o;</trd>,
    <trd>&c_k;&c_ib;&c_n;&c_a;&c_t;&c_e;&c_a;&c_t;&c_a;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kino</trd>,
    <trd>Filmtheater</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>movie theater</trd>,
    <trd>cinema</trd>,
    <trd>movie house</trd>
  </trdgrp>
  <trd lng="fr">cin&eacute;ma<klr> (lieu)</klr></trd>
  <trd lng="he">&bet;&yod;&tav; &qof;&vav;&lamed;&nun;&vav;&ayin;</trd>
  <trd lng="nl">bioscoop</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_n;&c_o;</trd>,
    <trd>&c_k;&c_i;&c_n;&c_o;&c_t;&c_je;&c_a;&c_t;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="teatr.pup0o">
  <kap>pup<tld/>o</kap>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="teatr.0o.belarto">Teatro</ref>
      en kiu rolas pupoj:
      <ekz>
        la &ccirc;ina pup<tld/>o havas historion de pli ol 2000 jaroj
        <fnt><bib>EncCxi</bib>, <lok>&ccirc;apitro &leftquot;Pupteatro&rightquot;, p.
        232a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_l;&c_je;&c_ch;&c_n;&c_y; <ind>&c_t;&c_e;&c_a;&c_t;&c_a;&c_r;</ind></trd>
  <trdgrp lng="fr">
    <trd>th&eacute;&acirc;tre de marionnettes</trd>
  </trdgrp>
  <trd lng="hu">b&aacute;bsz&iacute;nh&aacute;z</trd>
  <trd lng="nl">poppentheater</trd>
  <trd lng="de">Puppentheater</trd>
  <trd lng="ru">&c_k;&c_u;&c_k;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j; <ind>&c_t;&c_je;&c_a;&c_t;&c_r;</ind></trd>
</drv>
</art>
