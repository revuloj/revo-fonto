  <snc mrk="teatr.film0o.kinejo">
    <uzo tip="fak">KIN</uzo>
    <dif>
      <ref tip="dif" cel="kin.0ejo">Kinejo</ref>:
      <ekz>
        solena remalfermo en la <nom>Merkur</nom>-film<tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=453186">Popolaltlernejo
          Arnstadt-Ilmenau</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        kion fari el tiu kino<tld/>o en rilato kun kinoarto
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="&Monato;/2014/011204.php">La plej malnova
          kinoteatro de la mondo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_n;&c_a;&c_t;&c_e;&c_a;&c_t;&c_a;&c_r;</trd>
