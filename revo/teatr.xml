  </trdgrp>
  <trd lng="hu">sz&iacute;nh&aacute;z</trd>
  <trd lng="id">teater</trd>
  <trdgrp lng="ja">
    <trd>&#21127;&#22580;  <pr>&#12370;&#x304D;&#12376;&#12423;&#12358;</pr></trd>,
    <trd>&#x821E;&#x53F0;  <pr>&#12406;&#x305F;&#12356;</pr></trd>,
    <trd>&#21127;&#x56E3;  <pr>&#12370;&#x304D;&#12384;&#12435;</pr></trd>,
    <trd>&#x6F14;&#21127;  <pr>&#12360;&#12435;&#12370;&#x304D;</pr></trd>,
    <trd>&#x829D;&#x5C45;  <pr>&#12375;&#12400;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>theater</trd>,
    <trd>toneel</trd>
  </trdgrp>
  <trd lng="pl">teatr</trd>
  <trd lng="pt">teatro</trd>
  <trd lng="ru">&c_t;&c_je;&c_a;&c_t;&c_r;</trd>
  <trd lng="sk">divadlo</trd>
  <trd lng="sv">teater</trd>
  <trd lng="uk">&c_t;&c_je;&c_a;&c_t;&c_r;</trd>
</drv>

<drv mrk="teatr.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="teatr.0a.teatrajxo">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Rilata al la <tld/>o, la arto a&ubreve; la domo:
      <ekz>
        <tld/>a direktoro
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        dancistinoj <tld/>aj
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a prezentado
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;parmonujo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Theater-</trd>
  </snc>
  <snc mrk="teatr.0a.figure">
    <uzo tip="stl">FIG</uzo>
    <dif>
     Konvena al aktoro, anta&ubreve;kalkulita por fari efekton:
      <ekz>
        <tld/>aj manieroj
        <fnt><bib>F</bib>, <lok>&FE; 32</lok></fnt>;
      </ekz>
      <ekz>
        li kun <tld/>a gesto montris la pordon de la kabineto
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiun <tld/>an pozon akompanis viza&gcirc;esprimo, kiu
        signifis anka&ubreve; <tld/>an kaj tre komike aspektantan ravitecon
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">theatralisch</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>th&eacute;&acirc;tral</trd>
    <trd>th&eacute;&acirc;trale</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#21127;&#22580;&#x306E;  <pr>&#12370;&#x304D;&#12376;&#12423;&#12358;&#x306E;</pr></trd>,
    <trd>&#21127;&#x306E;  <pr>&#12370;&#x304D;&#x306E;</pr></trd>,
    <trd>&#x6F14;&#21127;&#x306E;  <pr>&#12360;&#12435;&#12370;&#x304D;&#x306E;</pr></trd>,
    <trd>&#x829D;&#x5C45;&#x304C;&#x304B;&#12387;&#x305F;  <pr>&#12375;&#12400;&#12356;&#x304C;&#x304B;&#12387;&#x305F;</pr></trd>
  </trdgrp>
  <trd lng="pl">teatralny</trd>
  <trd lng="uk">&c_t;&c_je;&c_a;&c_t;&c_r;&c_a;&#x43B;&#x44C;&#x43D;&c_i;&c_j;</trd>
</drv>

