  </trdgrp>
  <trd lng="es">cine</trd>
  <trd lng="fr">cin&eacute;matographe</trd>
  <trd lng="hu">filmsz&iacute;nh&aacute;z</trd>
  <trd lng="nl">bioscoop</trd>
  <trd lng="pl">kino</trd>
  <trd lng="ru">&c_k;&c_i;&c_n;&c_o;&c_t;&c_je;&c_a;&c_t;&c_r;</trd>
  <trd lng="sk">kino</trd>
  <trd lng="uk">&#x43A;&#x456;&#x43D;&#x43E;&#x442;&#x435;&#x430;&#x442;&#x440;</trd>
</drv>

<drv mrk="teatr.ombro0o">
  <kap>ombro<tld/>o</kap>
  <snc mrk="teatr.ombro0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      <tld lit="T"/>o, en kiu rolulojn
      prezentas ombraj figuroj projekciataj sur ekranon:
      <ekz>
        akademiuloj asertas, ke ombro<tld/>o venis al Anatolio en la
        16-a jarcento el Egiptio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1766">Islamo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Longdong</nom>-ombro<tld/>o tre furoris en la dinastioj
        <nom>Ming</nom> kaj <nom>Qing</nom>
        <klr>[&#x2026;]</klr> de la 14-a jarcento &gcirc;is la 19-a jarcento
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="&CRI;chinaabc/chapter21/chapter210101.htm">
          Ombroteatro</url></vrk>,
          <lok>&Ccirc;ina abc, [vidita en 2013]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Schattentheater</trd>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_a;&c_t;&c_a;&c_r; &c_c;&c_je;&c_n;&c_ja;&c_w;</trd>
  <trd lng="cs">st&iacute;nov&eacute; divadlo</trd>
  <trdgrp lng="es">
    <trd>teatro de sombras chinescas</trd>,
    <trd>teatro de sombras chinas</trd>,
    <trd>teatro de sombras</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ombres chinoises</trd>,
    <trd>th&eacute;&acirc;tre d'ombres</trd>
  </trdgrp>
  <trd lng="pl">teatr cieni</trd>
  <trd lng="sk">tie&ncaron;ov&eacute; divadlo</trd>
  <trd lng="uk">&#x442;&#x435;&#x430;&#x442;&#x440; &#x442;&#x456;&#x43D;&#x435;&#x439;</trd>
</drv>

<drv mrk="teatr.pup0o">
  <kap>pup<tld/>o,
    <var><kap>pupa <tld/>o</kap></var>
  </kap>
  <snc mrk="teatr.pup0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="super" cel="teatr.0o">Teatro</ref>,
      en kiu rolas pupoj:
      <ekz>
        la &ccirc;ina pup<tld/>o havas historion de pli ol 2000 jaroj
        <fnt><bib>EncCxi</bib>, <lok>&ccirc;apitro
        &leftquot;Pupteatro&rightquot;, p. 232a</lok></fnt>;
      </ekz>
      <ekz>
        malgranda pupa <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tistino kaj kamentubisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malgranda pup<tld/>o tuj estis starigita tiel, ke oni povis
        rekte enrigardi
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;parmonujo</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la granda drink&ccirc;ambro de la gastejo estis tre multe da homoj,
        &ccirc;ar tie trovi&gcirc;is pup<tld/>isto
        <klr>[&#x2026;]</klr>, li &jcirc;us starigis sian malgrandan <tld/>on, kaj la
        homoj sidis &ccirc;irka&ubreve;e, por rigardi la komedion
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_l;&c_je;&c_ch;&c_n;&c_y; <ind>&c_t;&c_e;&c_a;&c_t;&c_a;&c_r;</ind></trd>
  <trd lng="cs">loutkov&eacute; divadlo</trd>
  <trd lng="de">Puppentheater</trd>
