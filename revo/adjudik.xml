<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>adjudik</rad>/i
</kap>
<drv mrk="adjudik.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">JUR</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <fnt><bib>IKEV</bib></fnt>
  <snc>
    <dif>
      Proponi kontrakton pri laboro, livero ks al pluraj
      konkurencantoj, por ke ili proponu &gcirc;ian plenumon
      kontra&ubreve; plejeble malalta prezo kaj je alta kvalito
      a&ubreve; aliaj kondi&ccirc;oj.
    </dif>
    <ref tip="vid" cel="jugx.al0i">alju&gcirc;i</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;'&c_ja;&c_v;&c_ib;&c_c;&c_mol; <ind>&c_t;&c_e;&c_n;&c_d;&c_e;&c_r;</ind></trd>,
    <trd>&c_a;&c_b;'&c_ja;&c_v;&c_ib;&c_c;&c_mol; <ind>&c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_s;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ausschreiben</trd>,
    <trd>verdingen<klr> [ark]</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>invite tenders (for)</trd>,
    <trd>put out to tender</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire un appel d'<ind>offre</ind>s (pour)</trd>,
    <trd>mettre en <ind>adjudication</ind></trd>,
    <trd>mettre aux <ind>ench&egrave;re</ind>s</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tendert &iacute;r ki</trd>,
    <trd>versenyt&aacute;rgyal&aacute;st hirdet</trd>
  </trdgrp>
  <trd lng="sk">vyhl&aacute;si&tcaron; <ind>konkurz</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_malmol;&c_ja;&c_v;&c_i;&c_t;&c_mol; <ind>&c_t;&c_je;&c_n;&c_d;&c_je;&c_r;</ind></trd>,
    <trd>&c_o;&c_b;&c_malmol;&c_ja;&c_v;&c_i;&c_t;&c_mol; <ind>&c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_s;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="adjudik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">JUR</uzo>
  <fnt><bib>IKEV</bib></fnt>
  <snc>
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_e;&c_n;&c_d;&c_e;&c_r; <klr tip="amb">(&c_n;&c_a; &c_z;&c_a;&c_k;&c_u;&c_p;&c_k;&c_u;)</klr></trd>,
    <trd>&c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_s; <klr tip="amb">(&c_n;&c_a; &c_z;&c_a;&c_k;&c_u;&c_p;&c_k;&c_u;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ausschreibung</trd>,
    <trd>Verdingung<klr> [ark]</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>invitation to tender</trd>,
    <trd>tender invitation</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>appel d'<ind>offre</ind>s</trd>,
    <trd>adjudication</trd>,
    <trd><ind>ench&egrave;re</ind>s</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tenderki&iacute;r&aacute;s</trd>,
    <trd>versenyki&iacute;r&aacute;s</trd>
  </trdgrp>
  <trd lng="sk">konkurz</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_je;&c_n;&c_d;&c_je;&c_r;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_s; <klr tip="amb">(&c_n;&c_a; &c_z;&c_a;&c_k;&c_u;&c_p;&c_k;&c_u;, &c_r;&c_a;&c_b;&c_o;&c_t;&c_u;)</klr></trd>
  </trdgrp>
</drv>
</art>
