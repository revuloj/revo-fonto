<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: adjudik.xml,v 1.21 2023/10/27 13:42:09 revo Exp $">
<kap>
  <rad>adjudik</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="adjudik.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">JUR</uzo>
  <fnt><bib>IKEV</bib></fnt>
  <snc mrk="adjudik.0i.JUR">
    <dif>
      <ref tip="super" cel="propon.0i">Proponi</ref> kontrakton pri laboro, livero ks al pluraj
      konkurencantoj, por ke ili proponu &gcirc;ian plenumon
      kontra&ubreve; plejeble malalta prezo kaj je alta kvalito
      a&ubreve; aliaj kondi&ccirc;oj:
      <ekz>
        en decembro 1937 &Ccirc;inio tutmonde <tld/>is la konstruon de la &scirc;oseo
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="&Monato;/2011/010328.php">Fragmentoj de la dua mondmilito</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="jugx.al0i">alju&gcirc;i</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;'&c_ja;&c_v;&c_ib;&c_c;&c_mol; <ind>&c_t;&c_e;&c_n;&c_d;&c_e;&c_r;</ind></trd>,
    <trd>&c_a;&c_b;'&c_ja;&c_v;&c_ib;&c_c;&c_mol; <ind>&c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_s;</ind></trd>
  </trdgrp>
  <trd lng="cs">vypsat ve&rcaron;ejnou sout&ecaron;&zcaron;</trd>
  <trdgrp lng="de">
    <trd>ausschreiben</trd>,
    <trd>verdingen<klr> [ark]</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>invite tenders (for)</trd>,
    <trd>put out to tender</trd>
  </trdgrp>
  <trdgrp lng="es">
     <trd>adjudicar</trd>,
     <trd>contratar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire un appel d'<ind>offre</ind>s (pour)</trd>,
    <trd>mettre en <ind>adjudication</ind></trd>,
    <trd>mettre aux <ind>ench&egrave;re</ind>s</trd>
  </trdgrp>
  <trd lng="he">&lamed;&#x5B4;&pe;&#x5B0;&samekh;&vav;&#x5B9;&qof;</trd>
  <trdgrp lng="hu">
    <trd>tendert &iacute;r ki</trd>,
    <trd>versenyt&aacute;rgyal&aacute;st hirdet</trd>
  </trdgrp>
  <trd lng="it">appaltare</trd>
  <trd lng="pl">przetarg og&lstroke;asza&cacute;</trd>
  <trd lng="ro">inscriere</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_malmol;&c_ja;&c_v;&c_i;&c_t;&c_mol; <ind>&c_t;&c_je;&c_n;&c_d;&c_je;&c_r;</ind></trd>,
    <trd>&c_o;&c_b;&c_malmol;&c_ja;&c_v;&c_i;&c_t;&c_mol; <ind>&c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_s;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vyhl&aacute;si&tcaron; <ind>konkurz</ind></trd>,
    <trd>vyp&iacute;sa&tcaron; verejn&uacute; s&uacute;&tcaron;a&zcaron;</trd>
  </trdgrp>
  <trd lng="tr">ihaleye &ccedil;&inodot;karmak</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_i;&c_s;&c_u;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &c_p;&c_r;&c_a;&c_v;&c_o; (&c_n;&c_a; &c_o;&c_s;&c_n;&c_o;&c_v;&c_ib; &c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_s;&c_u;</trd>,
    <trd>&c_t;&c_je;&c_n;&c_d;&c_je;&c_r;&c_u;)</trd>
  </trdgrp>
</drv>

<drv mrk="adjudik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">JUR</uzo>
  <fnt><bib>IKEV</bib></fnt>
  <snc mrk="adjudik.0o.JUR">
    <dif>
      Ago <tld/>i:
      <ekz>
        la homoj <klr>[&#x2026;]</klr> estis muelistoj de <nom>Poket</nom>,
        veturantaj al <tld/>o pri liverado de faruno al
        armeo
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/vojo.html">
          La vojo nenien</url></vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="konkur2.acxet0o">a&ccirc;etkonkurso</ref>
    <ref tip="vid" cel="lot.0o.0ajxo">loto<sncref/></ref>
    <trdgrp lng="zh">
    <trd>&#x516C;&#x5F00;&#x6295;&#x6807; <pr>g&#x14D;ngk&#x101;it&#xF3;ubi&#x101;o</pr></trd>,
    <trd>&#x516C;&#x958B;&#x6295;&#x6A19; <pr>g&#x14D;ngk&#x101;it&#xF3;ubi&#x101;o</pr></trd>,
    <trd>&#x516C;&#x5F00;&#x62DB;&#x6807; <pr>g&#x14D;ngk&#x101;izh&#x101;obi&#x101;o</pr></trd>,
    <trd>&#x516C;&#x958B;&#x62DB;&#x6A19; <pr>g&#x14D;ngk&#x101;izh&#x101;obi&#x101;o</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_e;&c_n;&c_d;&c_e;&c_r; <klr tip="amb">(&c_n;&c_a; &c_z;&c_a;&c_k;&c_u;&c_p;&c_k;&c_u;)</klr></trd>,
    <trd>&c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_s; <klr tip="amb">(&c_n;&c_a; &c_z;&c_a;&c_k;&c_u;&c_p;&c_k;&c_u;)</klr></trd>
  </trdgrp>
  <trd lng="cs">vyps&aacute;n&iacute; ve&rcaron;ejn&eacute; sout&ecaron;&zcaron;e</trd>
  <trdgrp lng="de">
    <trd>Ausschreibung</trd>,
    <trd>Verdingung<klr> [ark]</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>invitation to tender</trd>,
    <trd>tender invitation</trd>
  </trdgrp>
  <trdgrp lng="es">
     <trd>adjudicaci&oacute;n</trd>,
     <trd>contrato</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>appel d'<ind>offre</ind>s</trd>,
    <trd>adjudication</trd>,
    <trd><ind>ench&egrave;re</ind>s</trd>
  </trdgrp>
  <trd lng="he">&shin;&#x5C1;&#x5B0;&pe;&#x5B4;&yod;&tet;&#x5B8;&he;</trd>
  <trdgrp lng="hu">
    <trd>tenderki&iacute;r&aacute;s</trd>,
    <trd>versenyki&iacute;r&aacute;s</trd>
  </trdgrp>
  <trd lng="it">appalto</trd>
  <trd lng="pl">przetarg</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_je;&c_n;&c_d;&c_je;&c_r;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_s; <klr tip="amb">(&c_n;&c_a; &c_z;&c_a;&c_k;&c_u;&c_p;&c_k;&c_u;,
    &c_r;&c_a;&c_b;&c_o;&c_t;&c_u;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>konkurz</trd>,
    <trd>vyp&iacute;sanie verejnej s&uacute;&tcaron;a&zcaron;e</trd>
  </trdgrp>
  <trd lng="tr">ihale</trd>
</drv>
</art>
