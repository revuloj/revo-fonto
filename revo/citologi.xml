<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>citologi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="citologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="citologi.0o.SCI">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="vid" cel="cxel.0o.BIO">&Ccirc;elo</ref>scienco:
      <ekz>
        eblos senpage <klr>...</klr> fari anka&ubreve; pli komplikajn 
        medicinajn ekzamenojn kiel <tld/>on a&ubreve; spirometrion
        <fnt>
          <aut>-</aut>,
          <vrk><url
          ref="http://www.polskieradio.pl/eo/dokument.aspx?iid=53287">
          Monda Tago sen Tabako</url></vrk>,
          <lok>Pola Radio, 2007-05-31</lok>
        </fnt>;
      </ekz>
      <ekz>
        Painter estis profesoro de <tld/>o en la universitato de Teksaso
        <fnt>
          <aut>Be&ccirc;jo</aut>,
          <vrk><url ref="http://becxjo.blogspot.com/">
          La mondo el mia vidpunkto</url></vrk>,
          <lok>2008-01-11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">cytologie</trd>
  <trd lng="pl">cytologia</trd>
  <trd lng="pt">citologia</trd>
</drv>

<drv mrk="citologi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="citologi.0isto.SCI">
    <dif>
      &Ccirc;elosciencisto.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>cytologiste</trd>,
    <trd>cytologue</trd>
  </trdgrp>
  <trd lng="pl">cytolog</trd>
  <trd lng="pt">citologista</trd>
</drv>
</art>

