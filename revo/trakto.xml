<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>traktor</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="trakto.0o">
  <kap><tld/>o</kap>
  <snc mrk="trakto.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Motorhava veturilo por kampokulturo a&ubreve; por treni aliajn
      veturilojn sur la kampo.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_a;&c_k;&c_t;&c_a;&c_r;</trd>
  <trd lng="bg">&c_t;&c_r;&c_a;&c_k;&c_t;&c_o;&c_r;</trd>
  <trd lng="de">Traktor</trd>
  <trd lng="en">tractor</trd>
  <trd lng="fr">tracteur</trd>
  <trdgrp lng="hu">
    <trd>traktor</trd>,
    <trd>vontat&oacute;</trd>
  </trdgrp>
  <trd lng="nl">tractor</trd>
  <trdgrp lng="pl">
    <trd>traktor</trd>,
    <trd>ci&aogonek;gnik<klr> rolniczy</klr></trd>
  </trdgrp>
  <trd lng="pt">trator</trd>
  <trd lng="ru">&c_t;&c_r;&c_a;&c_k;&c_t;&c_o;&c_r;</trd>
</drv>
</art>
