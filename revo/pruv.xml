    <drv mrk="pruv.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="pruv.0i.KOMUNE_iu">
        <uzo tip="klr">(iu)</uzo>
        <dif>
          Montri per rezonado a&ubreve; fakto, ke io estas vera:
          <ekz>
            <tld/>i la ekziston a&ubreve; neekziston de Dio, sian
            senkulpecon<fnt>B</fnt>;
          </ekz><ekz>
            mi <tld/>os al vi, ke vi eraras.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="demons.0i">demonstri</ref>,
          <ref cel="demons1">demonstracii</ref>
        </refgrp>
      </snc>
      <snc mrk="pruv.0i.KOMUNE_io">
        <uzo tip="klr">(io)</uzo>
        <dif>
          Esti la <tld/>o de:
          <ekz>
            tio <tld/>as la facilan lerneblecon de Esperanto;
          </ekz><ekz>
            tiu ekspozicio elokvente <tld/>as pri la progresoj faritaj;
          </ekz><ekz>
            larmoj pravecon ne <tld/>as
            <fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            nigro sur blanko <klr>(skriba&jcirc;o)</klr> <tld/>as sen
            manko
            <fnt><bib>PrV</bib></fnt>.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="argume">argumentadi</ref>,
          <ref cel="dokume.0i">dokumenti</ref>,
          <ref cel="ilustr.0i">ilustri</ref>,
          <ref cel="pled.0i">pledi</ref>
        </refgrp>
      </snc>
      <snc mrk="pruv.0i.MAT">
        <uzo tip="fak">MAT</uzo>
        <fnt><bib>PV</bib></fnt>
        <dif>
          Plene kaj nepre montri la verecon de matematika
          <ref cel="propoz.0o.aserto">propozicio</ref> per dedukto:
          <ekz>
            <tld/>i geometrian teoremon.
          </ekz>
        </dif>
        <ref tip="sin" cel="demons.0i.MAT">demonstri.</ref>
      </snc>
      <trd lng="af">bewys</trd>
  <trd lng="am">&#x12A0;&#x1228;&#x130B;&#x130D;&#x1325;</trd>
  <trd lng="ar">&#x625;&#x62B;&#x628;&#x627;&#x62A;</trd>
  <trd lng="az">s&#xFC;but etm&#x259;k</trd>
  <trdgrp lng="be">
        <trd>&c_d;&c_a;&c_v;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>,
        <trd>&c_d;&c_a;&c_k;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>
      </trdgrp>
      <trd lng="bn">&#x9AA;&#x9CD;&#x9B0;&#x9AE;&#x9BE;&#x9A3;</trd>
  <trd lng="bs">dokazati</trd>
  <trd lng="co">determin&#xE0;</trd>
  <trdgrp lng="cs">
        <trd>dok&aacute;zat</trd>,
        <trd>dolo&zcaron;it</trd>,
        <trd>prok&aacute;zat</trd>
      </trdgrp>
      <trd lng="da">bevise</trd>
  <trd lng="de">beweisen</trd>
      <trd lng="en">prove</trd>
      <trdgrp lng="es">
        <trd>probar</trd>,
        <trd>demostrar</trd>
      </trdgrp>
      <trd lng="et">t&#xF5;estama</trd>
  <trd lng="eu">frogatzeko</trd>
  <trdgrp lng="fr">
        <trd>prouver</trd>,
        <trd>d&eacute;montrer</trd>
      </trdgrp>
      <trd lng="fy">bewize</trd>
  <trd lng="gl">probar</trd>
  <trd lng="gu">&#xAB8;&#xABE;&#xAAC;&#xABF;&#xAA4;</trd>
  <trd lng="ha">tabbatar da</trd>
  <trd lng="hi">&#x938;&#x93E;&#x92C;&#x93F;&#x924;</trd>
  <trd lng="hr">dokazati</trd>
  <trdgrp lng="hu">
        <trd>bizony&iacute;t</trd>,
        <trd>igazol</trd>
      </trdgrp>
      <trd lng="hy">&#x561;&#x57A;&#x561;&#x581;&#x578;&#x582;&#x581;&#x565;&#x56C;</trd>
  <trd lng="is">sanna</trd>
  <trd lng="ja">&#x8A3C;&#x660E;&#x3059;&#x308B;</trd>
  <trd lng="jw">mbuktekaken</trd>
  <trd lng="ka">&#x10D3;&#x10D0;&#x10D0;&#x10DB;&#x10E2;&#x10D9;&#x10D8;&#x10EA;&#x10DD;&#x10E1;</trd>
  <trd lng="km">&#x1794;&#x1784;&#x17D2;&#x17A0;&#x17B6;&#x1789;</trd>
  <trd lng="ko">&#xC99D;&#xBA85;</trd>
  <trd lng="lo">&#xE9E;&#xEB4;&#xEAA;&#xEB9;&#xE94;</trd>
  <trd lng="ml">&#xD24;&#xD46;&#xD33;&#xD3F;&#xD2F;&#xD3F;&#xD15;&#xD4D;&#xD15;&#xD41;&#xD15;</trd>
  <trd lng="mr">&#x938;&#x93F;&#x926;&#x94D;&#x927;</trd>
  <trd lng="my">&#x101E;&#x1000;&#x103A;&#x101E;&#x1031;&#x1015;&#x103C;</trd>
  <trd lng="ne">&#x92A;&#x94D;&#x930;&#x92E;&#x93E;&#x923;&#x93F;&#x924;</trd>
  <trd lng="nl">bewijzen</trd>
      <trd lng="ny">kutsimikizira</trd>
  <trd lng="pa">&#xA38;&#xA3E;&#xA2C;&#xA24;</trd>
  <trd lng="pl">dowodzi&cacute;</trd>
      <trd lng="ps">&#x62B;&#x627;&#x628;&#x62A;</trd>
  <trdgrp lng="pt">
        <trd>provar <klr>(demonstrar)</klr></trd>,  
        <trd>demonstrar</trd>,
        <trd>fazer <ind>f&eacute;</ind></trd>,    
