      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_d;&c_o;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_d;&c_o;&c_k;&c_a;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
      <trd lng="sd">&#x62B;&#x627;&#x628;&#x62A;</trd>
