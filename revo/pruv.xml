  <trdgrp lng="sk">
        <trd>dok&aacute;za&tcaron;</trd>,
        <trd>dolo&zcaron;i&tcaron;</trd>,
        <trd>preuk&aacute;za&tcaron;</trd>
      </trdgrp>
      <trd lng="sq">provoj</trd>
  <trd lng="st">ba</trd>
  <trd lng="sv">bevisa</trd>
