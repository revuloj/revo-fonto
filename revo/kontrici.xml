<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kontrici.xml,v 1.7 2023/12/03 09:58:57 revo Exp $">
<kap>
  <rad>kontrici</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kontrici.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="kontrici.0o.PSI">
    <dif>
      <ref tip="dif" cel="pent.0o.PSI">Pento</ref>:
      <ekz>
        la renegato eniris denove en la sinon de la Eklezio <klr>[&#x2026;]</klr>
        dank' al siaj <tld/>o kaj pentofarado
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 65a, p.
        769a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_k;&c_a;&c_ja;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>l&iacute;tost</trd>,
    <trd>&zcaron;al</trd>
  </trdgrp>
  <trd lng="de">Bu&szlig;e</trd>
  <trd lng="fr">contrition</trd>
  <trdgrp lng="pl">
    <trd>skrucha</trd>,
    <trd>&zdot;al</trd>
  </trdgrp>   
  <trd lng="ro">remu&#x219;care</trd>
  <trdgrp lng="sk">
    <trd>&lcaron;&uacute;tos&tcaron;</trd>,
    <trd>&zcaron;ia&lcaron;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x81EA;&#x7F5A; <pr>z&#xEC;f&#xE1;</pr></trd>,
    <trd>&#x8D16;&#x7F6A; <pr>sh&#xFA;zu&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>

