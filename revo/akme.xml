<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: akme.xml,v 1.14 2023/10/28 08:33:03 revo Exp $">
<kap>
   <rad>akme</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="akme.0o">
   <kap><tld/>o</kap>
   <snc mrk="akme.0o.MED">
     <uzo tip="fak">MED</uzo>
     <dif>
       <ref tip="super" cel="paroks.0o">Paroksismo</ref> de malsano,
       kiam simptomoj intense aperas.
     </dif>
     <trd lng="de">Akme</trd>
     <trd lng="es">acm&eacute;</trd>
     <trd lng="pl">akme</trd>
     <trd lng="ru">&c_k;&c_r;&c_i;&c_z;&c_i;&c_s;<klr tip="amb"> (&c_b;&c_o;&c_l;&c_je;&c_z;&c_n;&c_i;)</klr></trd>
  </snc>
  <snc mrk="akme.0o.FIG">
    <dif>
      <ref tip="dif" cel="apoge.0o.FIG">Apogeo<sncref/></ref>,
      <ref tip="dif" cel="flor.0o.FIG">floro<sncref/></ref> (foje ironie):
      <ekz>
        la <tld/>o de la klasikaj filozofoj estis ordinare fiksita de la antikvaj
        historiistoj je la 40-a jara&gcirc;o
        <fnt>
          <aut>Evaldo Pauli</aut>
          <vrk><url ref="http://www.eventoj.hu/steb/vortaroj/filozofia-vortaro/AKILO-1.html">Enciklopedio  Pri  Filozofio</url></vrk>
          <lok>A, 0145</lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;Jak en &gcirc;el'&rightquot;, jen <tld/>o de perversa kuirado, kiun la a&ubreve;toro mokas per la vortoj
        &leftquot;vanto&rightquot;, &leftquot;pala helo&rightquot;, &leftquot;te-fe&ccirc;o&rightquot;;
        <fnt>
          <vrk><url ref="https://bulteno.esperanto-usa.org/a/1985/23/00-pdf/bulteno.pdf">Nova Fru-Esperanta Mansukripto, Parto II</url></vrk>
          <lok>JEN, A&ubreve;g-Okt 1985, vol 1, p. 6</lok>
        </fnt>;
      </ekz>
       <ekz>
         &scirc;i trovi&gcirc;is en <tld/>o de kolero
         <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 77a, p. 620a</lok></fnt>.
       </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="flor.0ado.prosperado">florado<sncref/></ref>,
      <ref cel="kulmin.0o.FIG">kulmino<sncref/></ref>,
      <ref cel="zenit.0o.FIG">zenito<sncref/></ref>
    </refgrp>
     <trd lng="de">H&ouml;hepunkt</trd>
     <trdgrp lng="pl">
     <trd>szczyt</trd>,
     <trd>punkt <ind>kulminacyjny</ind></trd>,
     <trd>kulminacja</trd>
     </trdgrp>
     <trd lng="ru">&c_k;&c_u;&c_l;&c_mol;&c_m;&c_i;&c_n;&c_a;&c_c;&c_i;&c_ja;</trd>
    <trd lng="zh">&#x9AD8;&#x6F6E; <pr>g&#x101;och&#xE1;o</pr></trd>
</snc>
  <trd lng="be">&c_a;&c_k;&c_m;&c_e;</trd>
  <trdgrp lng="cs">
    <trd>akme - vyvrcholen&iacute;</trd>,
    <trd>akm&eacute;</trd>
  </trdgrp>
  <trd lng="fr">acm&eacute;</trd>
  <trd lng="grc">&alfa_psili;&kappa;&my;&eta_ton;</trd>
  <trd lng="pt">acme</trd>
  <trd lng="sk">vyvrcholenie</trd>
  <trd lng="uk">&c_a;&c_k;&c_m;&c_je;</trd>
</drv>
</art>

