<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: testud.xml,v 1.35 2024/08/14 18:32:30 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>testud</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="testud.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="testud.0o.ZOO">
    <dif>
      <ref tip="lst" cel="reptil.0o" lst="voko:reptilioj">Reptilio</ref>
      el la ordo <ref tip="malprt" cel="testud.0oj"><tld/>oj</ref>,
      kies tuta korpo, estas entenata en korneca kiraso el kiu
      povas eli&gcirc;i la kapo, la kvar piedoj kaj la vosto:
      <ekz>
        la malgrandaj <tld/>oj estis miaj kamaradoj de ludo
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> mar&scirc;as tiel malrapide kiel <tld/>o
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekunua</lok>
        </fnt>;
      </ekz>
      <ekz>
        Nero, enirinte, sidi&gcirc;is sur se&gcirc;on, inkrustitan per <tld/>a &scirc;elo
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XLI</lok>
        </fnt>;
      </ekz>
      <ekz>
        pro la <tld/>eco de la ordinara po&scirc;to unu triono de nia tuta tempo forglitas por
        la vo&ccirc;donado
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 3 (65)</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la jaro 1976 la mar&ccirc;a <tld/>o estis akceptata en la Ru&gcirc;an Libron de Litovio
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2012/010875.php"
              >Pereon de specioj malhelpas homoj</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_r;&c_a;&c_p;&c_a;&c_h;&c_a;</trd>
  <trd lng="bo">&t_ra;&t_shabchu;&t_sa;&t_tsheg;&t_sa;&t_bata;&t_la;&t_tsheg;</trd>
  <trd lng="ca">tortuga</trd>
  <trd lng="cs">testudo</trd>
  <trd lng="de">Schildkr&ouml;te</trd>
  <trdgrp lng="en">
    <trd>tortoise</trd>,
    <trd>turtle</trd>
  </trdgrp>
  <trd lng="es">tortuga</trd>
  <trd lng="fr">tortue</trd>
  <trdgrp lng="hu">
    <trd>tekn&odblac;s</trd>,
    <trd>tekn&odblac;sb&eacute;ka</trd>
  </trdgrp>
  <trd lng="id">kura-kura</trd>
  <trdgrp lng="ja">
    <trd>&#12459;&#12513;</trd>,
    <trd>&#20096;&#30002;&#22411;&#25513;&#33995; <pr>&#12365;&#12387;&#12371;&#12358;&#12363;&#12383;&#12360;&#12435;&#12364;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">schildpad</trd>
  <trd lng="no">skilpadde</trd>
  <trd lng="pl">&zdot;&oacute;&lstroke;w</trd>
  <trdgrp lng="pt">
    <trd>tartaruga</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_je;&c_p;&c_a;&c_h;&c_a;</trd>
  <trd lng="sk">korytna&ccaron;ka</trd>
  <trd lng="uk">&c_ch;&c_je;&c_r;&c_je;&c_p;&c_a;&c_h;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#40860; <pr>gu&imacron;</pr></trd>,
    <trd>&#40863; <pr>gu&imacron;</pr></trd>,
    <trd>&#29619; <pr>d&agrave;i</pr></trd>,
    <trd>&#20044;&#40863; <pr>w&umacron;gu&imacron;</pr></trd>,
    <trd>&#28879;&#40860; <pr>w&umacron;gu&imacron;</pr></trd>,
    <trd>&#20096; <pr>gu&imacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="testud.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="testud.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref>
      de reptilioj (<trd lng="la">Testudinata</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_r;&c_a;&c_p;&c_a;&c_h;&c_ib; <klr>(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="fr">Testudines</trd>
</drv>

<drv mrk="testud.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="testud.0ajxo.ZOO">
    <dif>
      Substanco konsistiganta kirason de maraj <tld/>oj.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_n;&c_c;&c_y;&c_r; <klr tip="amb">(&c_ch;&c_a;&c_r;&c_a;&c_p;&c_a;&c_sh;&c_y;)</klr></trd>
  <trd lng="ca">carei</trd>
  <trd lng="de">Schildpatt</trd>
  <trd lng="fr">&eacute;caille<klr tip="amb"> (substance)</klr></trd>
  <trdgrp lng="hu">
    <trd>tekn&odblac;ch&eacute;j</trd>,
    <trd>tekn&odblac;sp&aacute;nc&eacute;l</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12409;&#12387;&#30002;  <pr>&#12409;&#12387;&#12371;&#12358;</pr></trd>,
    <trd>&#12409;&#12387;&#30002;&#35069;&#21697;  <pr>&#12409;&#12387;&#12371;&#12358;&#12379;&#12356;&#12402;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">schildpad</trd>
  <trd lng="pl">szylkret</trd>
  <trd lng="uk">&c_ch;&c_je;&c_r;&c_je;&c_p;&c_a;&c_sh;&c_a;&c_ch;&c_i;&c_j; &c_p;&c_a;&c_n;&c_c;&c_i;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#29767;&#29761; <pr>d&agrave;im&egrave;i</pr></trd>,
    <trd>&#40860;&#27580; <pr>gu&imacron;k&eacute;</pr></trd>,
    <trd>&#40863;&#22771; <pr>gu&imacron;k&eacute;</pr></trd>,
    <trd>&#40860;&#30002; <pr>gu&imacron;ji&#462;</pr></trd>,
    <trd>&#40863;&#30002; <pr>gu&imacron;ji&#462;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="testud.0e">
  <kap><tld/>e</kap>
  <snc mrk="testud.0e.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kiel <tld/>o, tre malrapide:
      <ekz>
        bona famo sin trenas <tld/>e, malbona kuras rapide
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ja;&c_k; <ind>&c_ch;&c_a;&c_r;&c_a;&c_p;&c_a;&c_h;&c_a;</ind></trd>,
    <trd>&c_n;&c_ib;&c_b;&c_y; <ind>&c_ch;&c_a;&c_r;&c_a;&c_p;&c_a;&c_h;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="ca">a pas de <ind>tortuga</ind></trd>
  <trdgrp lng="cs">
    <trd>pomalu</trd>,
    <trd>vleklo</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schleichend <ind>langsam</ind></trd>,
    <trd>im <ind>Schneckentempo</ind></trd>,
    <trd>lahmarschig</trd>,
    <trd>im <ind>Zeitlupe</ind>ntempo</trd>,
    <trd>wie eine <ind>Schlaftablette</ind></trd>
  </trdgrp>
  <trd lng="fr">comme une <ind>tortue</ind></trd>
  <trd lng="hu">mint a <ind>csiga</ind></trd>
  <trd lng="ja">&#12398;&#12429;&#12398;&#12429;&#12392;</trd>
  <trd lng="nl">schildpadsgewijs</trd>
  <trd lng="pl">w &zdot;&oacute;&lstroke;wim tempie</trd>
  <trd lng="ru">
  &c_p;&c_o;-&c_ch;&c_je;&c_r;&c_je;&c_p;&c_a;&c_sh;&c_mol;&c_i;</trd>
  <trdgrp lng="sk">
    <trd>pomaly</trd>,
    <trd>vleklo</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;-&c_ch;&c_je;&c_r;&c_je;&c_p;&c_a;&c_sh;&c_a;&c_ch;&c_o;&c_m;&c_u;</trd>,
    <trd>&c_ja;&c_k; &c_ch;&c_je;&c_r;&c_je;&c_p;&c_a;&c_h;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="testud.verdamar0o">
  <kap>verda mar<tld/>o</kap>
  <snc mrk="testud.verdamar0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Mar<ref tip="super" cel="testud.0o"><tld/>o</ref>
      vivanta en tropikaj maroj, pezanta
      &gcirc;is 130 kilogramoj; genro kun unu
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:zoologiaj_specioj">specio</ref>
      (<trd lng="la">Chelonia mydas</trd>):
      <ekz>
        la <klr>[&#8230;]</klr> verda mar<tld/>o <klr>[&#8230;]</klr> sin trovas en
        la &ccirc;ina marbordo, precipe en insuletoj de la Suda
        &Ccirc;ina Maro
        <fnt><bib>Viki</bib>,
        <lok>artikolo &leftquot;kelonio&rightquot;, 2005-10-17</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_ja;&c_l;&c_jo;&c_n;&c_a;&c_ja; &c_m;&c_a;&c_r;&c_s;&c_k;&c_a;&c_ja; <ind>&c_ch;&c_a;&c_r;&c_a;&c_p;&c_a;&c_h;&c_a;</ind></trd>
  <trd lng="ca"><ind>tortuga</ind> verda</trd>
  <trd lng="de">Suppenschildkr&ouml;te</trd>
  <trd lng="en">green sea turtle</trd>
  <trdgrp lng="fr">
    <trd>tortue franche</trd>,
    <trd>tortue verte</trd>
  </trdgrp>
  <trd lng="nl">soepschildpad</trd>
  <trd lng="pl"><klr>jadalny </klr>&zdot;&oacute;&lstroke;w zielony
  </trd>
  <trd lng="pt">tartaruga verde</trd>
</drv>
</art>
