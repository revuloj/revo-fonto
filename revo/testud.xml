<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>testud</rad>/o
</kap>
<drv mrk="testud.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="ramp.0uloj">Rampulo</ref>,
      kies tuta korpo, estas entenata en korneca kiraso el kiu
      povas eli&gcirc;i la kapo, la kvar piedoj kaj la vosto:
      <ekz>
        la maraj
        <tld/>oj atingas pli ol unu metron da longo;
      </ekz>
      <ekz>
        pa&scirc;i kiel <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">schildpad</trd>
  <trd lng="pl">&zdot;&oacute;&lstroke;w</trd>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_je;&c_p;&c_a;&c_h;&c_a;</trd>
</drv>

<drv mrk="testud.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Substanco konsistiganta kirason de maraj <tld/>oj.
    </dif>
  </snc>
  <trd lng="nl">schildpad</trd>
</drv>

<drv mrk="testud.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Kiel <tld/>o, tre malrapide:
      <ekz>
        sin treni <tld/>e<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="limak.0o">limako</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">schildpadsgewijs</trd>
  <trd lng="ru">&c_p;&c_o;-&c_ch;&c_je;&c_r;&c_je;&c_p;&c_a;&c_sh;&c_mol;&c_i;</trd>
</drv>
</art>
