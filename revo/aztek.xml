<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>aztek</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="aztek.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="indian.0o">Indiano</ref> el popolo reginta
      la centron de nuna Meksiko &ccirc;irka&ubreve; Meksikurbo
      inter jaroj 1200a kaj 1521a:
      <ekz>
        la arkeologoj konsentas, ke la kulturo de la <tld/>oj kaj aliaj
        mezamerikaj popoloj ne estis originala, sed ja heredita de pli
        antikvaj popoloj<fnt><aut>Garbhan
        MacAoidh</aut>: <vrk>La ajmara</vrk>, <bib>Monato</bib><lok>jaro
        2002a, numero 2a, p. 26-28a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>Azt&egrave;que<klr> (subst.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="aztek.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>oj.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>azt&egrave;que<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
-->
</vortaro>