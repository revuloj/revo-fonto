<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kapeldiner.xml,v 1.3 2019/10/12 13:10:21 revo Exp $">
<kap>
  <rad>kapeldiner</rad>/o
</kap>
<drv mrk="kapeldiner.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="kapeldiner.0o.TUR">
    <dif>
      <ref tip="super" cel="serv.0isto">Servisto</ref> de
      <ref tip="malprt" cel="kapel.0o.MUZ">kapelo<sncref/></ref>:
      <ekz>
        iu titola konsilisto <klr>[&#8230;]</klr> starigis &ccirc;e la 
        pordo iujn <tld/>ojn kun galonoj kaj ru&gcirc;aj kolumoj
        <fnt>
          <aut>N. Gogol, trad. A. Kor&jcirc;enkov</aut>,
          <vrk><url ref="http://esperanto-ondo.ru/Rn-gogl2.htm">
          Palto</url></vrk>,
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <!-- formon arkaikan traduku prefere per arkaika formo nacilingva -->
  <trd lng="de">Kapelldiener</trd>
  <trd lng="en">commissionaire</trd>
  <trdgrp lng="fr">
    <trd>appariteur<klr> (portier)</klr></trd>,
    <trd>huissier<klr> (portier)</klr></trd>
  </trdgrp>
</drv>
</art>

