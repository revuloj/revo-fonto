<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>negatron</rad>/o, negaton/o
</kap>
<drv mrk="negaton.0o">
  <kap><tld/>o, negatono</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Negative &scirc;argita <ref tip="super" cel="elektr14.0o">elektrono</ref>,
      kontraste kun la <ref cel="poziton.0o">pozitrono</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Negaton</trd>,
     <trd>Negatron</trd>
  </trdgrp>
  <trd lng="en">negatron</trd>
  <trd lng="nl">negatron</trd>
  <trdgrp lng="ru">
    <trd><klr>&c_o;&c_t;&c_r;&c_i;&c_c;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</klr> &c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_n;</trd>,
    <trd>&c_n;&c_je;&c_g;&c_a;&c_t;&c_r;&c_o;&c_n;</trd>
  </trdgrp>
</drv>
</art>

