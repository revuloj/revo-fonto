<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>negaton</rad>/o
</kap>
<drv mrk="negaton.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Negative &scirc;argita <ref tip="super" cel="elektr14.0o">elektrono</ref>,
      kontraste kun la <ref cel="poziton.0o">pozitrono</ref>.
    </dif>
  </snc>
  <ref tip="sin" cel="negatr.0o">negatrono</ref>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_m;&c_o;&c_w;&c_n;&c_y; <ind>&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_n;</ind></trd>,
    <trd>&c_n;&c_e;&c_g;&c_a;&c_t;&c_r;&c_o;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Negaton</trd>,
    <trd>Negatron</trd>
  </trdgrp>
  <trdgrp lng="en"><trd>negaton</trd>, <trd>negatron</trd></trdgrp>
  <trdgrp lng="hu">
    <trd>negaton</trd>,
    <trd>negatron</trd>
  </trdgrp>
  <trdgrp lng="nl"><trd>negaton</trd>, <trd>elektron</trd></trdgrp>
  <trdgrp lng="pt">
    <trd>n&eacute;gatron</trd>,
    <trd>negatr&atilde;o <klr>(Port.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_r;&c_i;&c_c;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; <ind>&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_n;</ind></trd>,
    <trd>&c_n;&c_je;&c_g;&c_a;&c_t;&c_r;&c_o;&c_n;</trd>
  </trdgrp>
</drv>
</art>
