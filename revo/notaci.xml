    <kap>
      <rad>notaci</rad>/o
    </kap>

<drv mrk="notaci.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="konven1.0o.gxenerale">Konvencio</ref>
      la&ubreve; kiu oni skribas a&ubreve; desegnas instrukciojn per
      grafikaj elementoj:
      <ekz>
        &scirc;ak<tld/>o, danc<tld/>o;
      </ekz>
      <ekz>
        inversa pola <tld/>o;
      </ekz>
      <ekz>
        la plej vaste uzata muzika <tld/>o estas sufi&ccirc;e
        kompleksa kaj ne perfekta
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2004/008034.html">Klavarskribo</url></vrk>
               <bib>Monato</bib>
          <lok>2004/07, p. 26</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kod.0o.SCI">kodo<sncref/></ref>,
      <ref cel="simbol.0igi">simboligo</ref>,
      <ref cel="sintak.0o">sintakso</ref>,
      <ref cel="skrib.0o.sistemo">skribo<sncref/></ref>
    </refgrp>
    <trd lng="de">Notation</trd>
    <trd lng="en">notation</trd>
    <trd lng="fr">notation</trd>
    <trd lng="nl">notatie</trd>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_t;&c_a;&c_c;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_p;&c_o;&c_s;&c_a;&c_b; &c_z;&c_a;&c_p;&c_ib;&c_s;&c_u;)</klr></trd>
  <trdgrp lng="zh">
    <trd>&#x8A18;&#x8B5C;&#x6CD5; <pr>j&#xEC;p&#x1D4;f&#x1CE;</pr></trd>,
    <trd>&#x8BB0;&#x8C31;&#x6CD5; <pr>j&#xEC;p&#x1D4;f&#x1CE;</pr></trd>
  </trdgrp>
</drv>

  </art>

