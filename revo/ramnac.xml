<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ramnac.xml,v 1.10 2023/10/06 08:29:43 revo Exp $">
<kap><rad>ramnac</rad>/oj</kap>
<drv mrk="ramnac.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref> de <ref
      tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Rhamnaceae</trd>):
      <ekz>
        la zizifo estas genro de fal- a&ubreve; da&ubreve;ra-foliaj arboj kaj arbedoj el <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=50160">Zizifo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_u;&c_sh;&c_y;&c_n;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  <trd lng="ca">ramn&agrave;cies</trd>
  <trd lng="de">Kreuzdorngew&auml;chse</trd>
  <trdgrp lng="hu">
    <trd>varj&uacute;t&ouml;visf&eacute;l&eacute;k</trd>,
    <trd>bengef&eacute;l&eacute;k</trd>
  </trdgrp>
  <trd lng="pl">szak&lstroke;akowate</trd>
  <trd lng="pt">ramn&aacute;ceas</trd>
  <trd lng="ro">ramnacee</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_u;&c_sh;&c_i;&c_n;&c_o;&c_v;&c_y;&c_je;<klr tip="ind">
(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_k;&c_r;&c_u;&c_sh;&c_i;&c_n;&c_n;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="uk">&c_zh;&#x43E;&c_s;&c_t;&c_je;&c_r;&#x43E;&c_v;&c_ib;</trd>
</drv>
</art>

