<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>koit</rad>/o
</kap>
<drv mrk="koit.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZL</uzo>
  <snc>
    <dif>
      Sekskuni&gcirc;o, dum kiu viro elfluigas sian
      <ref tip="vid" cel="sperm.0o">spermon</ref>
      sen konsidero pri reprodukto:
      <ekz>natura, kontra&ubreve;natura, <ind>interrompita <tld/>o</ind>
    <trd lng="nl">coitus interruptus</trd>
    <trd lng="sk">preru&scaron;ovan&aacute; <ind>s&uacute;lo&zcaron;</ind></trd>
    </ekz>.
    </dif>
    <ref tip="sin" cel="kopula.0o">kopulacio</ref>
  </snc>
  <trdgrp lng="ca">
     <trd>coit</trd>,
     <trd>c&ograve;pula</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Koitus</trd>,
     <trd>Beischlaf</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>co&iuml;tus</trd>,
    <trd>geslachtsgemeenschap</trd>
  </trdgrp>
<trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_v;&c_o;&c_k;&c_u;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_o;&c_v;&c_o;&c_je; &c_s;&c_n;&c_o;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">s&uacute;lo&zcaron;</trd>
</drv>
<drv mrk="koit.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on.
    </dif>
  </snc>
  <trd lng="ca">copular</trd>
  <trdgrp lng="de">
     <trd>koitieren</trd>,
     <trd>den Beischlaf vollziehen</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>co&iuml;teren</trd>,
    <trd>geslachtsgemeenschap hebben</trd>
  </trdgrp>
<trd lng="ru">&c_s;&c_o;&c_v;&c_o;&c_k;&c_u;&c_p;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="sk">s&uacute;lo&zcaron;i&tcaron;</trd>
</drv>
</art>

