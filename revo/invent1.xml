<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>inventar</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="invent1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Detala etato, en kiu estas sinsekve enskribitaj &ccirc;iuj objektoj,
      varoj, valoroj apartenantaj al iu persono, societo, firmo.  
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Inventar</trd>,
    <trd>Bestandsliste</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>inventory</trd>,
    <trd>stocklist</trd>
  </trdgrp>
  <trd lng="nl">inventaris</trd>
  <trd lng="ru">&c_i;&c_n;&c_v;&c_je;&c_n;&c_t;&c_a;&c_r;&c_mol;</trd>
</drv>
<drv mrk="invent1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Starigi <tld/>on: <ekz><tld/>i siajn komerca&jcirc;ojn.</ekz>
    </dif>
  </snc>
  <trd lng="de">inventarisieren</trd>
  <trd lng="nl">inventariseren</trd>
  <trd lng="ru">&c_i;&c_n;&c_v;&c_je;&c_n;&c_t;&c_a;&c_r;&c_i;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
