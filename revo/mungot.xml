<vortaro>
  <art mrk="$Id: mungot.xml,v 1.6 2024/01/26 19:05:54 afido Exp $">
    <kap>
      <rad>mungot</rad>/o <fnt><bib>PIV1</bib></fnt>,
      <var><kap><rad var="g">mung</rad>/o</kap></var>
    </kap>

<drv mrk="mungot.0o">
  <kap><tld/>o,
    <var><kap><tld var="g"/>o</kap></var>
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Iu el genro <trd lng="la">Mungo</trd> k.a. genroj de 
      familio <trd lng="la">Herpestidae</trd> de malgrandaj 
      <ref tip="malprt" cel="best.rabo0oj">rabobestoj</ref> 
      vivantaj en Afriko kaj suda Azio,
      kun svelta korpo kaj mallongaj kruroj, grize bruna felo, 
      makulita a&ubreve; striita &ccirc;e kelkaj specioj, 
      malgranda kapo kaj oreloj kaj ne tre longa iom tufa vosto:
      <ekz>
        Havaja ansero <klr>[&#x2026;]</klr> preska&ubreve; formortis en 1952, kies ekzempleroj estis
        mortigitaj fare de <tld/>oj, porkoj kaj katoj, <klr>[&gcirc;is]</klr> nur 30 birdoj
        vivis
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=21020">Havaja ansero</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis <tld var="g"/>o, iom simila pro felo kaj vosto al kateto, sed kape kaj
        kutime musteleca, rozkoloraj estis liaj okuloj kaj la pinto de lia
        maltrankvila nazo
        <fnt>
          <aut>Rudyard Kipling, trad. Don Harlow</aut>
          <vrk><url ref="&DonH;Noveloj/Rikki_Tikki_Tavi.html">La &Gcirc;angala Libro</url></vrk>
          <lok>Rikki-Tikki-Tavi</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/d/d9/Vosmangoesten_zoo_Lille.JPG/640px-Vosmangoesten_zoo_Lille.JPG" prm="CC-BY-SA-3.0">Flava <tld/>o (Cynictis penicillata), besto&gcirc;ardeno de Lille, Francujo
        <fnt>
          <aut>Bios, propra verko</aut>
          <lok><url ref="&WCW;/File:Vosmangoesten_zoo_Lille.JPG">&WCW;/File:Vosmangoesten_zoo_Lille.JPG</url></lok>
        </fnt>
    </bld>    
    <trd lng="as">mangosta</trd>
    <trdgrp lng="be">
      <trd>&c_m;&c_a;&c_n;&c_g;&c_u;&c_s;&c_t;</trd>,
      <trd>&c_m;&c_a;&c_n;&c_g;&c_u;&c_s;&c_t;&c_a;</trd>
    </trdgrp>
    <trd lng="cs">mangusta</trd>
    <trd lng="de">Manguste</trd>
    <trd lng="en">mongoose</trd>
    <trd lng="es">mangosta</trd>
    <trd lng="fi">mangusti</trd>
    <trd lng="fr">mangouste</trd>
    <trd lng="hu">mong&uacute;z</trd>
    <trd lng="hy">&#x574;&#x561;&#x576;&#x563;&#x578;&#x582;&#x57D;&#x57F;</trd>
    <trd lng="id">musang</trd>
    <trd lng="it">mangusta</trd>
    <trd lng="kn">&#xCAE;&#xCC1;&#xC82;&#xC97;&#xCC1;&#xCB8;&#xCBF;</trd>
    <trd lng="ko">&#xB3C5;&#xC0AC;&#xB97C; &#xBA39;&#xC74C;</trd>
    <trd lng="lt">mangusta</trd>
    <trd lng="nl">mangoest</trd>
    <trd lng="ro">mangust&abreve;</trd>
    <trd lng="ru">&c_m;&c_a;&c_n;&c_g;&c_u;&c_s;&c_t;&c_a;</trd>
    <trd lng="sl">mungo</trd>
    <trd lng="sv">mungo</trd>
    <trd lng="sw">nguchiro</trd>
    <trd lng="te">&#xC2E;&#xC41;&#xC02;&#xC17;&#xC3F;&#xC38;</trd>
    <trd lng="th">&#xE1E;&#xE31;&#xE07;&#xE1E;&#xE2D;&#xE19;</trd>
    <trd lng="uk">&c_m;&c_a;&c_n;&c_g;&c_u;&c_s;&c_t;&c_a;</trd>
  </snc>
  <trd lng="ja">&#x30DE;&#x30F3;&#x30B0;&#x30FC;&#x30B9;</trd>
  <trdgrp lng="zh">
    <trd>&#x7374; <pr>m&#x11B;ng</pr></trd>,
    <trd>&#x7338;&#x5B50; <pr>m&#xE9;izi</pr></trd>,
    <trd>&#x68D5;&#x84D1;&#x732B; <pr>z&#x14D;ngsu&#x14D;m&#x101;o</pr></trd>,
    <trd>&#x68D5;&#x84D1;&#x8C93; <pr>z&#x14D;ngsu&#x14D;m&#x101;o</pr></trd>
