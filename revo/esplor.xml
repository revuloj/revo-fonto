<drv mrk="esplor.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="esplor.0i.KOMUNE">
    <dif>
      Atente rigardi por ekkoni ion nekonatan a&ubreve; ka&scirc;itan:
      <ekz>
        <tld/>i aferon, fenomenon, malsanulon, akuziton;
      </ekz>
      <ekz>
        <ind>al &ccirc;evalo donacita oni bu&scirc;on ne <tld/>as</ind>
        <trd lng="fr">&agrave; cheval donn&eacute; on ne regarde pas les
        dents</trd>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        &ccirc;iunokte la fi&scirc;kaptisto rigarde <tld/>as la aspekton
        de l'
        &ccirc;ielo<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i nekonatan landon;
      </ekz>
      <ekz>
        <tld/>i, kion ka&scirc;as la koro de knabino;
      </ekz>
      <ekz>
        <ind><tld/>a</ind> komitato
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 18a</lok></fnt>.
        <trd lng="fr">exploratoire</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekzame.0i">ekzameni</ref>,
      <ref cel="enket.0i">enketi</ref>,
      <ref cel="inform.0igxi">informi&gcirc;i</ref>,
      <ref cel="sercx.0i">ser&ccirc;i</ref>,
      <ref cel="sond.0i">sondi</ref>,
      <ref cel="stud.0i">studi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_s;&c_mol;&c_l;&c_je;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_s;&c_mol;&c_l;&c_je;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_v;&c_u;&c_ch;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&t_zha;&t_gigu;&t_ba;&t_tsheg;&t_-a;&t_ja;&t_shabchu;&t_ga;&t_tsheg;&t_ba;&t_yata;&t_drengbu;&t_da;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>b&aacute;dat</trd>,
    <trd>ohledat</trd>,
    <trd>ohled&aacute;vat</trd>,
    <trd>p&aacute;trat</trd>,
    <trd>vyzkou&scaron;et</trd>,
    <trd>vy&scaron;et&rcaron;it</trd>,
    <trd>vy&scaron;et&rcaron;ovat</trd>,
    <trd>zkoumat</trd>,
    <trd>zpytovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>erforschen</trd>,
    <trd>erkunden</trd>
  </trdgrp>
  <trd lng="en">explore</trd>
  <trdgrp lng="es">
    <trd>examinar</trd>,
    <trd>explorar</trd>,
    <trd>investigar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>examiner<klr tip="ind"> (&eacute;tudier)</klr></trd>,
    <trd>explorer</trd>,
    <trd>&eacute;tudier</trd>,
    <trd>faire une <ind>recherche</ind> (sur)</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kutat</trd>,
    <trd>vizsg&aacute;l</trd>,
    <trd>tanulm&aacute;nyoz</trd>
  </trdgrp>
  <trdgrp lng="ja">
