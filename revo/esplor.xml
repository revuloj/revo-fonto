    <trd>&#x79D1;&#x7814;&#x6D3B;&#x52A8; <pr>k&emacron;y&aacute;nhu&oacute;d&ograve;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="esplor.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="esplor.0ema.KOMUNE">
    <dif>
      Provanta bone, funde ekkoni per atenta observo:
      <ekz>
        li <tld/>eme rigardis mian viza&gcirc;on
        <fnt>
          <bib>VaK</bib>
          <lok>Kvara &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne <tld/>ema scivolo ka&ubreve;zis tiun demandon, estis nura deziro al interkompreni&gcirc;o
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la praaj religioj estis manifesto de <tld/>emo
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[en &scirc;iaj]</klr> okuloj <klr>[&#x2026;]</klr> aperis denove energio kaj <tld/>emeco
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="intere.0igxisin0i">interesita</ref>,
      <ref cel="sci.0volema">scivolema</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_d;&c_a;&c_p;&c_y;&c_t;&c_l;&c_ib;&c_v;&c_y;</trd>,
      <trd>&c_c;&c_ib;&c_k;&c_a;&c_w;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>curieux<klr tip="ind"> (investigateur)</klr></trd>,
      <trd>inquisiteur<klr tip="ind"> (adj.)</klr></trd>,
      <trd>investigateur<klr tip="ind"> (adj.)</klr></trd>,
      <trd>scrutateur<klr tip="ind"> (adj.)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>badawczy</trd>,
      <trd>dociekliwy</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>zkoumav&yacute;</trd>,
    <trd>zv&iacute;dav&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>forschend</trd>,
    <trd>wissbegierig</trd>,
    <trd>wissensdurstig </trd>,
    <trd>neugierig</trd>,
    <trd>interessiert</trd>
  </trdgrp>
  <trd lng="en">curious <klr>(inquisitive)</klr></trd>
  <trd lng="it">curioso</trd>
  <trdgrp lng="ja">
