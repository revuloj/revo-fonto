<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: esplor.xml,v 1.24 2022/03/17 13:47:19 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>esplor</rad>/i
</kap>
<drv mrk="esplor.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="esplor.0i.KOMUNE">
    <dif>
      Atente rigardi por ekkoni ion nekonatan a&ubreve; ka&scirc;itan:
      <ekz>
        <tld/>i aferon, fenomenon, malsanulon, akuziton;
      </ekz>
      <ekz>
        <ind>al &ccirc;evalo donacita oni bu&scirc;on ne <tld/>as</ind>
        <trd lng="fr">&agrave; cheval donn&eacute; on ne regarde pas les
        dents</trd>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        &ccirc;iunokte la fi&scirc;kaptisto rigarde <tld/>as la aspekton
        de l'
        &ccirc;ielo<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i nekonatan landon;
      </ekz>
      <ekz>
        <tld/>i, kion ka&scirc;as la koro de knabino;
      </ekz>
      <ekz>
        <ind><tld/>a</ind> komitato
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 18a</lok></fnt>.
        <trd lng="fr">exploratoire</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekzame.0i">ekzameni</ref>,
      <ref cel="enket.0i">enketi</ref>,
      <ref cel="inform.0igxi">informi&gcirc;i</ref>,
      <ref cel="sercx.0i">ser&ccirc;i</ref>,
      <ref cel="sond.0i">sondi</ref>,
      <ref cel="stud.0i">studi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_s;&c_mol;&c_l;&c_je;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_s;&c_mol;&c_l;&c_je;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_v;&c_u;&c_ch;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&#xF5E;&#xF72;&#xF56;&#xF0B;&#xF60;&#xF47;&#xF74;&#xF42;&#xF0B;&#xF56;&#xFB1;&#xF7A;&#xF51;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>b&aacute;dat</trd>,
    <trd>ohledat</trd>,
    <trd>ohled&aacute;vat</trd>,
    <trd>p&aacute;trat</trd>,
    <trd>vyzkou&scaron;et</trd>,
    <trd>vy&scaron;et&rcaron;it</trd>,
    <trd>vy&scaron;et&rcaron;ovat</trd>,
    <trd>zkoumat</trd>,
    <trd>zpytovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>erforschen</trd>,
    <trd>erkunden</trd>
  </trdgrp>
  <trd lng="en">explore</trd>
  <trdgrp lng="es">
    <trd>examinar</trd>,
    <trd>explorar</trd>,
    <trd>investigar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>examiner<klr tip="ind"> (&eacute;tudier)</klr></trd>,
    <trd>explorer</trd>,
    <trd>&eacute;tudier</trd>,
    <trd>faire une <ind>recherche</ind> (sur)</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kutat</trd>,
    <trd>vizsg&aacute;l</trd>,
    <trd>tanulm&aacute;nyoz</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bada&cacute;</trd>,
    <trd>docieka&cacute;</trd>,
    <trd>dochodzi&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>explorar</trd>,
    <trd>pesquisar</trd>,
    <trd>buscar</trd>,
    <trd>escavar</trd>,
    <trd>investigar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_z;&c_u;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>b&aacute;da&tcaron;</trd>,
    <trd>p&aacute;tra&tcaron;</trd>,
    <trd>sk&uacute;ma&tcaron;</trd>,
    <trd>vy&scaron;etrova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x434;&#x43E;&#x441;&#x43B;&#x456;&#x434;&#x436;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x438;&#x432;&#x447;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x440;&#x43E;&#x437;&#x441;&#x43B;&#x456;&#x434;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x438;&#x441;&#x43B;&#x443;&#x445;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43E;&#x431;&#x441;&#x43B;&#x443;&#x445;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43E;&#x431;&#x441;&#x442;&#x435;&#x436;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="esplor.0o">
  <kap><tld/>o</kap>
  <snc mrk="esplor.0o.KOMUNE">
    <dif>
      Atenta rigardado, zorga provo de ekkono:
      <ekz>
        por fremda koro ne ekzistas <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        li faris historian <tld/>on en la Biblioteko Vatikana
        <fnt><bib>Gerda</bib>, <lok>&ccirc;apitro 24a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>&eacute;tude</trd>,
      <trd>examen<klr tip="ind"> (&eacute;tude)</klr></trd>,
      <trd>exploration</trd>,
      <trd>recherche<klr tip="ind"> (scientifique)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>badanie</trd>,
      <trd>&sacute;ledztwo</trd>,
      <trd>dochodzenie</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_s;&c_mol;&c_l;&c_je;&c_d;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="bo">&#xF5E;&#xF72;&#xF56;&#xF0B;&#xF60;&#xF47;&#xF74;&#xF42;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd><klr>(vy)</klr>&scaron;et&rcaron;en&iacute;</trd>,
    <trd>ohled&aacute;n&iacute;</trd>,
    <trd>vy&scaron;et&rcaron;ov&aacute;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>examen</trd>,
    <trd>exploraci&oacute;n</trd>,
    <trd>investigaci&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vy&scaron;etrovanie</trd>,
    <trd>&scaron;etrenie</trd>
  </trdgrp>
</drv>

<drv mrk="esplor.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="esplor.0ema.KOMUNE">
    <dif>
      Provanta bone, funde ekkoni per atenta observo:
      <ekz>
        <ind><tld/>ema</ind> rigardo.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_a;&c_p;&c_y;&c_t;&c_l;&c_ib;&c_v;&c_y;</trd>,
      <trd>&c_c;&c_ib;&c_k;&c_a;&c_w;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>curieux<klr tip="ind"> (investigateur)</klr></trd>,
      <trd>inquisiteur<klr tip="ind"> (adj.)</klr></trd>,
      <trd>investigateur<klr tip="ind"> (adj.)</klr></trd>,
      <trd>scrutateur<klr tip="ind"> (adj.)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>badawczy</trd>,
      <trd>dociekliwy</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>zkoumav&yacute;</trd>,
    <trd>zv&iacute;dav&yacute;</trd>
  </trdgrp>
  <trd lng="en">curious</trd>
  <trd lng="it">curioso</trd>
  <trd lng="sk">sk&uacute;mav&yacute;</trd>
</drv>
</art>
