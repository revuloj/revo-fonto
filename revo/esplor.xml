<drv mrk="esplor.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="esplor.0i.KOMUNE">
    <dif>
      Atente rigardi por ekkoni ion nekonatan a&ubreve; ka&scirc;itan:
      <ekz>
        <tld/>u la aferon
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>u por ni la landon
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 1:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni <tld/>os viajn vortojn, &ccirc;u vi diris la veron
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 42:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is la ka&scirc;itajn fortojn <klr>[&#x2026;]</klr> de la metaloj
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        li komencis <tld/>i singarde la plankon per la piedo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        ta&ccirc;mentoj de rajdistoj <tld/>is la &ccirc;irka&ubreve;a&jcirc;on en &ccirc;iuj direktoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni devos plu <tld/>i pri la murdo
        <fnt>
          <bib>CKv</bib>
          <lok>9</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is iom la fundon de la glaso
        <klr>(ebrii&gcirc;is)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>al &ccirc;evalo donacita oni bu&scirc;on ne <tld/>as</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="de"><mll tip="kom">einem geschenkten <ind>Gaul</ind></mll> schaut man nicht ins Maul</trd>
        <trd lng="fr">&agrave; cheval donn&eacute; on ne regarde pas les
        dents</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekzame.0i">ekzameni</ref>,
      <ref cel="enket.0i">enketi</ref>,
      <ref cel="inform.0igxi">informi&gcirc;i</ref>,
      <ref cel="sercx.0i">ser&ccirc;i</ref>,
      <ref cel="sond.0i">sondi</ref>,
      <ref cel="stud.0i">studi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_s;&c_mol;&c_l;&c_je;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_s;&c_mol;&c_l;&c_je;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_v;&c_u;&c_ch;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&t_zha;&t_gigu;&t_ba;&t_tsheg;&t_-a;&t_ja;&t_shabchu;&t_ga;&t_tsheg;&t_ba;&t_yata;&t_drengbu;&t_da;&t_tsheg;</trd>
  <trdgrp lng="cs">
