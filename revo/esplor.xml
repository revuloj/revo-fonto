<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>esplor</rad>/i
</kap>
<drv mrk="esplor.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="esplor.0i.KOMUNE">
    <dif>
      Atente rigardi por ekkoni ion nekonatan a&ubreve; ka&scirc;itan:
      <ekz>
        <tld/>i aferon, fenomenon, malsanulon, akuziton;
      </ekz>
      <ekz>
        <ind>al &ccirc;evalo donacita oni bu&scirc;on ne <tld/>as</ind>
        <trd lng="fr">&agrave; cheval donn&eacute; on ne regarde pas les
        dents</trd>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        &ccirc;iunokte la fi&scirc;kaptisto rigarde <tld/>as la aspekton
        de l'
        &ccirc;ielo<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i nekonatan landon;
      </ekz>
      <ekz>
        <tld/>i, kion ka&scirc;as la koro de knabino;
      </ekz>
      <ekz>
        <ind><tld/>a</ind> komitato
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 18a</lok></fnt>.
        <trd lng="fr">exploratoire</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekzame.0i">ekzameni</ref>,
      <ref cel="enket.0i">enketi</ref>,
      <ref cel="inform.0igxi">informi&gcirc;i</ref>,
      <ref cel="sercx.0i">ser&ccirc;i</ref>,
      <ref cel="sond.0i">sondi</ref>,
      <ref cel="stud.0i">studi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_s;&c_mol;&c_l;&c_je;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_s;&c_mol;&c_l;&c_je;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_v;&c_u;&c_ch;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>erforschen</trd>,
    <trd>erkunden</trd>
  </trdgrp>
  <trd lng="en">explore</trd>
  <trdgrp lng="fr">
    <trd>examiner<klr tip="ind"> (&eacute;tudier)</klr></trd>,
    <trd>explorer</trd>,
    <trd>&eacute;tudier</trd>,
    <trd>faire une <ind>recherche</ind> (sur)</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kutat</trd>,
    <trd>vizsg&aacute;l</trd>,
    <trd>tanulm&aacute;nyoz</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bada&cacute;</trd>,
    <trd>docieka&cacute;</trd>,
    <trd>dochodzi&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>explorar</trd>,
    <trd>pesquisar</trd>,
    <trd>buscar</trd>,
    <trd>escavar</trd>,
    <trd>investigar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_z;&c_u;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bádať</trd>,
    <trd>pátrať</trd>,
    <trd>skúmať</trd>,
    <trd>vyšetrovať</trd>
  </trdgrp>
</drv>

<drv mrk="esplor.0o">
  <kap><tld/>o</kap>
  <snc mrk="esplor.0o.KOMUNE">
    <dif>
      Atenta rigardado, zorga provo de ekkono:
      <ekz>
        por fremda koro ne ekzistas <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        li faris historian <tld/>on en la Biblioteko Vatikana
        <fnt><bib>Gerda</bib>, <lok>&ccirc;apitro 24a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>&eacute;tude</trd>,
      <trd>examen<klr tip="ind"> (&eacute;tude)</klr></trd>,
      <trd>exploration</trd>,
      <trd>recherche<klr tip="ind"> (scientifique)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>badanie</trd>,
      <trd>&sacute;ledztwo</trd>,
      <trd>dochodzenie</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="sk">
    <trd>vyšetrovanie</trd>,
    <trd>šetrenie</trd>
  </trdgrp>
</drv>

<drv mrk="esplor.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="esplor.0ema.KOMUNE">
    <dif>
      Provanta bone, funde ekkoni per atenta observo:
      <ekz>
        <ind><tld/>ema</ind> rigardo.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>curieux<klr tip="ind"> (investigateur)</klr></trd>,
      <trd>inquisiteur<klr tip="ind"> (adj.)</klr></trd>,
      <trd>investigateur<klr tip="ind"> (adj.)</klr></trd>,
      <trd>scrutateur<klr tip="ind"> (adj.)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>badawczy</trd>,
      <trd>dociekliwy</trd>
    </trdgrp>
  </snc>
  <trd lng="sk">skúmavý</trd>
</drv>
</art>
