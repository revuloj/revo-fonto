    <pr>&#12379;&#12435;&#12373;&#12367;&#12377;&#12365;&#12398;</pr></trd>,
    <trd>&#25506;&#31350;&#24515;&#12395;&#23500;&#12435;&#12384;
    <pr>&#12383;&#12435;&#12365;&#12421;&#12358;&#12371;&#12371;&#12429;&#12395;&#12392;&#12435;&#12384;</pr></trd>
  </trdgrp>
  <trd lng="sk">sk&uacute;mav&yacute;</trd>
  <trd lng="zh">&#22909;&#22855; <pr>h&agrave;oq&iacute;</pr></trd>
