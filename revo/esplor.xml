<drv mrk="esplor.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      <tld/>anta; komisiita al <tld/>o:
      <ekz>
        <tld/>a ju&gcirc;isto
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>a</ind> komitato
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 18a</lok></fnt>;
        <trd lng="fr">exploratoire</trd>
      </ekz>
      <ekz>
        rigardo atenta, preska&ubreve; <tld/>a
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a laboro
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Untersuchungs-</trd>,
    <trd>Forschungs-</trd>,
    <trd>forschend</trd>
  </trdgrp>
</drv>

<drv mrk="esplor.0o">
  <kap><tld/>o</kap>
  <snc mrk="esplor.0o.KOMUNE">
    <dif>
      Atenta rigardado, zorga provo de ekkono:
      <ekz>
        fari pri li <tld/>on
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 23:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ju&gcirc;a <tld/>o restas sen rezultato
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        li faris historian <tld/>on en la Biblioteko Vatikana
        <fnt><bib>Gerda</bib>, <lok>&ccirc;apitro 24a</lok></fnt>;
      </ekz>
      <ekz>
        pri la postmorta vivo la pastroj dum jarmilaj <tld/>oj eksciis multajn gravajn
        detalojn
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        logika <tld/>o de la afero montras al ni, ke <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista en Washington en la 15a de a&ubreve;gusto 1910</vrk>
        </fnt>;
      </ekz>
      <ekz>
        por fremda koro ne ekzistas <tld/>o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>&eacute;tude</trd>,
      <trd>examen<klr tip="ind"> (&eacute;tude)</klr></trd>,
      <trd>exploration</trd>,
      <trd>recherche<klr tip="ind"> (scientifique)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>badanie</trd>,
      <trd>&sacute;ledztwo</trd>,
      <trd>dochodzenie</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_s;&c_mol;&c_l;&c_je;&c_d;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="bo">&t_zha;&t_gigu;&t_ba;&t_tsheg;&t_-a;&t_ja;&t_shabchu;&t_ga;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd><klr>(vy)</klr>&scaron;et&rcaron;en&iacute;</trd>,
    <trd>ohled&aacute;n&iacute;</trd>,
    <trd>vy&scaron;et&rcaron;ov&aacute;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Erkundung</trd>,
    <trd>Untersuchung</trd>,
    <trd>Recherche</trd>,
    <trd>Nachforschung</trd>
  </trdgrp>
  <trd lng="en">exploration</trd>
  <trdgrp lng="es">
    <trd>examen</trd>,
    <trd>exploraci&oacute;n</trd>,
    <trd>investigaci&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8E0F;&#x67FB;  <pr>&#x3068;&#x3046;&#x3055;</pr></trd>,
    <trd>&#x8ABF;&#x67FB;  <pr>&#x3061;&#x3087;&#x3046;&#x3055;</pr></trd>,
    <trd>&#x63A2;&#x7A76;  <pr>&#x305F;&#x3093;&#x304D;&#x3085;&#x3046;</pr></trd>,
    <trd>&#x7814;&#x7A76;  <pr>&#x3051;&#x3093;&#x304D;&#x3085;&#x3046;</pr></trd>,
    <trd>&#x8A3A;&#x67FB;  <pr>&#x3057;&#x3093;&#x3055;</pr></trd>,
    <trd>&#x5BE9;&#x7406;  <pr>&#x3057;&#x3093;&#x308A;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vy&scaron;etrovanie</trd>,
    <trd>&scaron;etrenie</trd>
  </trdgrp>
  <trdgrp lng="zh">
