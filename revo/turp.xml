<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: turp.xml,v 1.8 2022/05/05 17:08:12 revo Exp $">
<kap>
  <rad>turp</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="turp.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="turp.0a.KOMUNE">
    <dif>
      <ref tip="dif" cel="bel.mal0a.KOMUNE">Malbelega</ref>:
      <ekz>
        la stratoj estis senhomaj, <tld/>aj, la kafejoj malplenaj kaj neniu kura&gcirc;is
        eniri en ilin
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/007140.html">de Julian Modest</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        belon kaj <tld/>on, &ccirc;ielon kaj inferon
        vi &ccirc;erpis nure el spirito mia
        <fnt>
          <aut>I. Mad&aacute;ch, trad. K. Kalocsay</aut>, 
          <vrk>La Tragedio de l' Homo</vrk>, 
          <lok>1965</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">o&scaron;kliv&yacute;</trd>
  <trdgrp lng="de">
    <trd>h&auml;&szlig;lich</trd>,
    <trd>potth&auml;&szlig;lich</trd>
  </trdgrp>
  <trd lng="en">ugly</trd>
  <trd lng="fr">hideux</trd>
  <trdgrp lng="pl">
    <trd>brzydki</trd>,
    <trd>szkaradny</trd>
  </trdgrp>
  <trd lng="sk">&scaron;kared&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x43D;&#x435;&#x43A;&#x440;&#x430;&#x441;&#x438;&#x432;&#x438;&#x439;</trd>,
    <trd>&#x43D;&#x435;&#x433;&#x430;&#x440;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x431;&#x440;&#x438;&#x434;&#x43A;&#x438;&#x439;</trd>
  </trdgrp>
</drv>
</art>

