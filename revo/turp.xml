<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: turp.xml,v 1.10 2023/10/06 08:29:45 revo Exp $">
<kap>
  <rad>turp</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="turp.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="turp.0a.KOMUNE">
    <dif>
      <ref tip="dif" cel="bel.mal0a.KOMUNE">Malbelega</ref>:
      <ekz>
        la stratoj estis senhomaj, <tld/>aj, la kafejoj malplenaj kaj neniu kura&gcirc;is
        eniri en ilin
        <fnt>
          <aut>Julian Modest</aut>
            <vrk><url ref="&Monato;/2001/007140.html">Vetiha</url></vrk>
            <bib>Monato</bib>
            <lok>2001/02, p. 26</lok>
          </fnt>;
      </ekz>
      <ekz>
        belon kaj <tld/>on, &ccirc;ielon kaj inferon
        vi &ccirc;erpis nure el spirito mia
        <fnt>
          <aut>I. Mad&aacute;ch, trad. K. Kalocsay</aut>,
          <vrk>La Tragedio de l' Homo</vrk>,
          <lok>1965</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_y;&c_d;&c_k;&c_ib;</trd>
  <trd lng="cs">o&scaron;kliv&yacute;</trd>
  <trdgrp lng="de">
    <trd>h&auml;&szlig;lich</trd>,
    <trd>potth&auml;&szlig;lich</trd>
  </trdgrp>
  <trd lng="en">ugly</trd>
  <trd lng="fr">hideux</trd>
  <trdgrp lng="pl">
    <trd>brzydki</trd>,
    <trd>szkaradny</trd>
  </trdgrp>
  <trd lng="sk">&scaron;kared&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_je;&#x43A;&c_r;&c_a;&c_s;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&#x43D;&c_je;&c_g;&c_a;&c_r;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_r;&c_i;&c_d;&#x43A;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>

