<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>apoteoz</rad>/i
</kap>
<drv mrk="apoteo.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Diigi.
    </dif>
    <trdgrp lng="fr">
      <trd>d&eacute;ifier</trd>,
      <trd>diviniser</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Gloregi per egaj la&ubreve;doj.
    </dif>
    <trd lng="fr">porter aux nues</trd>
  </snc>
  <trd lng="ru">&c_b;&c_o;&c_g;&c_o;&c_t;&c_v;&c_o;&c_r;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="apoteo.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Diigo.
    </dif>
  </snc>
  <snc>
    <dif>
      Glorego.
    </dif>
  </snc>
  <trd lng="ru">&c_a;&c_p;&c_o;&c_f;&c_je;&c_o;&c_z;</trd>
  <trdgrp lng="fr">
    <trd>apoth&eacute;ose</trd>,
    <trd>cons&eacute;cration</trd>,
    <trd>triomphe</trd>
  </trdgrp>
</drv>
</art>
