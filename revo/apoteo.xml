<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: apoteo.xml,v 1.23 2025/03/27 06:26:25 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>apoteoz</rad>/i
<fnt><bib>JED</bib></fnt></kap>
<drv mrk="apoteo.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Diigi.
    </dif>
    <trdgrp lng="de">
      <trd>verg&ouml;ttlichen</trd>,
      <trd>verg&ouml;ttern</trd>,
      <trd>zum Gott <ind>erheben</ind></trd>,
      <trd>zu einem <ind>Gott</ind> machen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;ifier</trd>,
      <trd>diviniser</trd>
    </trdgrp>
    <trd lng="nl">vergoddelijken</trd>
  </snc>
  <snc>
    <dif>
      <ref tip="sin" cel="glor.0i">Gloregi</ref>, 
      <ref tip="sin" cel="ador.0i.religie">adoregi</ref>:
      <ekz>
        en la urbo restas kelkaj &scirc;intoaj temploj kiuj <tld/>as <nac>Heian</nac>-epokan
        (la 12-jarcentan) generalon <nom>Kiso Yosinaka</nom>   
        <fnt>
          <vrk><url ref="http://www.ilei.info/novajxoj/Ranzan.php">La 
          40-a Konferenco de ILEI en Ranzan (Japanio)</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        lia spirito estis <tld/>ita en la kapeleto <nom>Senrej&scirc;a</nom>
        <fnt>
          <vrk><url ref="http://www.oomoto.or.jp/Esperanto/esDokt/demres.html">Demandoj 
          kaj Respondoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ejo
        <fnt>
          <vrk><url ref="http://www.oomoto.or.jp/Esperanto/esDokt/demres.html">Demandoj kaj
          Respondoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>glorifizieren</trd>,
      <trd>verg&ouml;ttern</trd>,
      <trd>in den <ind>Himmel</ind> heben</trd>
    </trdgrp>
    <trd lng="fr">porter aux nues</trd>
    <trd lng="nl">verheerlijken</trd>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_a;&c_g;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>glorifikovat</trd>,
    <trd>zbo&zcaron;nit</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>apothrosize</trd>,
    <trd>deify</trd>,
  </trdgrp>
  <trdgrp lng="hu">
    <trd>felmagasztal</trd>,
    <trd>isten&iacute;t</trd>,
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#31070;&#12395;&#31085;&#12427;  <pr>&#12363;&#12415;&#12395;&#12414;&#12388;&#12427;</pr></trd>,
    <trd>&#12354;&#12364;&#12417;&#12427;</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_o;&c_g;&c_o;&c_t;&c_v;&c_o;&c_r;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>nad&scaron;ene zbo&zcaron;&ncaron;ova&tcaron;</trd>,
    <trd>oslavova&tcaron;</trd>,
    <trd>ospevova&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="apoteo.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Diigo.
    </dif>
    <trdgrp lng="de">
      <trd>Verg&ouml;ttlichung</trd>,
      <trd>Apotheose</trd>
    </trdgrp>
    <trd lng="nl">vergoddelijking</trd>
  </snc>
  <snc>
    <dif>
      Glorego:
      <ekz>
        estas nun l' <tld/>o de funebra flor'
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Glorifizierung</trd>,
      <trd>Verg&ouml;tterung</trd>
    </trdgrp>
    <trd lng="nl">verheerlijking</trd>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Anka&#365; eblas diri <ke>diigo</ke>, <ke>glorigo</ke>, <ke>triumfo</ke>. 
  </rim>

  <trd lng="be">&c_a;&c_p;&c_a;&c_t;&c_e;&c_o;&c_z;</trd>
  <trdgrp lng="cs">
    <trd>apote&oacute;za</trd>,
    <trd>glorifikace</trd>,
    <trd>zbo&zcaron;n&ecaron;n&iacute;</trd>
  </trdgrp>
  <trd lng="en">apotheosis</trd>
  <trdgrp lng="fr">
    <trd>apoth&eacute;ose</trd>,
    <trd>cons&eacute;cration</trd>,
    <trd>triomphe</trd>
  </trdgrp>
  <trd lng="he">&#1492;&#1463;&#1488;&#1458;&#1500;&#1464;&#1492;&#1464;&#1492;</trd>
  <trdgrp lng="hu">
    <trd>megdics&odblac;&uuml;l&eacute;s</trd>,
    <trd>felmagasztal&aacute;s</trd>,
    <trd>apote&oacute;zis</trd>
  </trdgrp>
  <trd lng="ja">&#31070;&#26684;&#21270;  <pr>&#12375;&#12435;&#12363;&#12367;&#12363;</pr></trd>
  <trdgrp lng="nl">
    <trd>apotheose</trd>,
  </trdgrp>
  <trd lng="ru">&c_a;&c_p;&c_o;&c_f;&c_je;&c_o;&c_z;</trd>
  <trd lng="sk">glorifik&aacute;cia</trd>
  <trdgrp lng="uk">
    <trd>&#1072;&#1087;&#1086;&#1092;&#1077;&#1086;&#1079;</trd>,
    <trd>&#1087;&#1088;&#1086;&#1089;&#1083;&#1072;&#1074;&#1083;&#1103;&#1085;&#1085;&#1103;</trd>
  </trdgrp>
</drv>
</art>
