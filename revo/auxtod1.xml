<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: auxtod1.xml,v 1.19 2021/03/04 18:15:49 revo Exp $">
<kap>
  <rad>a&ubreve;todidakt</rad>/o
</kap>

<drv mrk="auxtod1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PED</uzo>
  <snc mrk="auxtod1.0o.memlernanto">
    <dif>
      <refgrp tip="dif">
        <ref cel="lern.mem0anto">Memlernanto</ref>,
        <ref cel="lern.mem0into">memlerninto</ref>
      </refgrp>:
      <ekz>
        <ctl>Esperanto por <tld/>oj</ctl>
        <klr>(titolo de <ref cel="lern.mem0ilo">memlernilo</ref> pri
        Esperanto)</klr>
        <fnt>
          <vrk>Prelegaro</vrk>
          <lok>Internacia Seminario pri Komenio, Prago,
          &Ccirc;EA, 1992</lok>
        </fnt>;
      </ekz><ekz>
        temis pri <ctl>Saluton!</ctl>, la <tld/>a lernilo de
        <nom>Audrey Childs-Mee</nom>
        <fnt>
          <vrk>UEA: furora vendojaro 1999</vrk>
          <bib>LOdE</bib>, <lok>2000. N-ro 4</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Lanti</nom> <klr>[&#x2026;]</klr> estis <tld/>o, memlerninto
        <fnt>
          <aut>Georges Lagrange</aut>:
          <vrk>Privat kaj Lanti</vrk>.
          <bib>LOdE</bib>, <lok>1992:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekspozicio pri arto de <tld/>oj: skulpta&jcirc;oj, pentra&jcirc;oj, fotoj
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="https://www.monato.be/2005/008666.php">Folklora festivalo en Kaustinen</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a konstruisto inventis &gcirc;in
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Straznickas</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006877.html">Nekutimaj transportiloj</url></vrk>
        </fnt>.
      </ekz>  
    </dif>  
  </snc>
  <trd lng="be">&c_s;&c_a;&c_m;&c_a;&c_v;&c_u;&c_ch;&c_k;&c_a;</trd>
  <trd lng="cs">samouk</trd>
  <trd lng="de">Autodidakt</trd>
  <trdgrp lng="en">
    <trd>self-taught person</trd>,
    <trd>autodidact</trd>
  </trdgrp>
  <trd lng="es">autodidacta</trd>
  <trd lng="fr">autodidacte<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="he">
    <trd>&#x5D0;&#x5D5;&#x5D8;&#x5D5;&#x5D3;&#x5D9;&#x5D3;&#x5E7;&#x5D8;</trd>,
    <trd>&#x5DC;&#x5D5;&#x5DE;&#x5D3; &#x5D1;&#x5E2;&#x5E6;&#x5DE;&#x5D5;</trd>
  </trdgrp>
  <trd lng="hu">autodidakta</trd>
  <trd lng="it">autodidatta</trd>
  <trd lng="nl">autodidact</trd>
  <trd lng="pl">samouk</trd>
  <trd lng="ru">&c_s;&c_a;&c_m;&c_o;&c_u;&c_ch;&c_k;&c_a;</trd>
  <trd lng="sk">samouk</trd>
</drv>

<drv mrk="auxtod1.0e">
  <kap>
    <tld/>e
  </kap>
  <snc mrk="auxtod1.0e.memstare">
    <dif>
      Lernante memstare:
      <ekz>
        li lernis <tld/>e la lingvon 
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="https://www.monato.be/2007/009010.php">Resume de l' mio</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>e akiris multajn konojn krom tiuj de profesia agitisto en politiko, li
        tamen lernis retorikon
        <fnt>
          <bib>Monato</bib>
          <aut>Gilbert Ledon</aut>
          <vrk><url ref="https://www.monato.be/2003/008015.html">Misa mondo</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi estas hungaro, studanta <tld/>e, kaj volas lerni la vortojn anglalingvajn kaj
        esperantajn pri hejmaj a&ubreve; familiaj aferoj
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="https://www.monato.be/2006/008956.php">Unika vortaro por lernantoj</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">autodidaktisch</trd>
  <trd lng="he">&#x5D0;&#x5D5;&#x5D8;&#x5D5;&#x5D3;&#x5D9;&#x5D3;&#x5E7;&#x5D8;</trd>
</drv>


</art>
