<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: asklep.xml,v 1.21 2024/09/04 01:49:42 revo Exp $">
<kap>
  <rad>asklepiad</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="asklep.0o">
  <kap><tld/>o</kap>
  <snc mrk="asklep.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio <tld/>acoj, en kiu abundas
      blanka laktosimila suko (<trd lng="la">Asclepias</trd>):
      <ekz>
        sur marbordaj dunoj vegeta&jcirc;aro estas malofta, je la piedo da krestoj, aliflanke,
        grandaj tamarisk-arbustoj, nanakacioj, kaj <tld/>oj povas esti trovitaj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=428948">Geografio de Ma&ubreve;ritanio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/2e/Asclepias_cordifolia.JPG/324px-Asclepias_cordifolia.JPG" prm="CC    
    BY-SA 3.0">floraro de <tld/>o (Asclepias cordifolia)
        <fnt>
          <aut>First Light, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Asclepias_cordifolia.JPG">&WCW;/File:Asclepias_cordifolia.JPG</url></lok>
        </fnt>
    </bld>
    <trd lng="en">milkweed</trd>
    <trd lng="de">Seidenpflanze</trd>
    <trdgrp lng="fr">
      <trd>ascl&eacute;piade</trd>,
      <trd>ascl&eacute;pias</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>selyemk&oacute;r&oacute;</trd>,
      <trd>krepin</trd></trdgrp>
    <trd lng="it">asclepiadi</trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_a;&c_t;&c_o;&c_ch;&c_n;&c_i;&c_k;</trd>,
      <trd>&c_l;&c_a;&c_s;&c_t;&c_o;&c_ch;&c_n;&c_i;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x841D;&#x85E6;&#x79D1; <pr>lu&#xF3;m&#xF3;k&emacron;</pr></trd>,
    <trd>&#x863F;&#x85E6;&#x79D1; <pr>lu&#xF3;m&#xF3;k&emacron;</pr></trd>
  </trdgrp>
</snc>
  <ref tip="hom" cel="asklep1.0o">asklepiado (versmetro)</ref>
  <trd lng="be">&c_v;&c_a;&c_t;&c_a;&c_ch;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>asklep</trd>,
    <trd>asklepiadick&yacute; ver&scaron;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30C8;&#x30A6;&#x30EF;&#x30BF;</trd>,
    <trd>&#x30A2;&#x30B9;&#x30AF;&#x30EC;&#x30D4;&#x30A2;&#x30C7;&#x30B9;&#x683C; 
    <pr>&#x30A2;&#x30B9;&#x30AF;&#x30EC;&#x30D4;&#x30A2;&#x30C7;&#x30B9;&#x304B;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="pl">troje&sacute;&cacute;</trd>
  <trdgrp lng="sk">
    <trd>asklep</trd>,
    <trd>asklepiatick&yacute; ver&scaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_a;&c_t;&#x43E;&c_ch;&#x43D;&c_i;&#x43A;</trd>
</drv>
</art>
