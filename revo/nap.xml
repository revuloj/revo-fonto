<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nap.xml,v 1.34 2024/01/29 15:21:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>nap</rad>/o
</kap>
<drv mrk="nap.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">AGR</uzo>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_specioj" cel="speci.0o">Specio</ref>
      de <ref tip="super" cel="brasik.0o">brasiko</ref>
      (<trd lng="la">Brassica napus</trd>);
      la tuberaj radikoj de la supspecio  (<trd lng="la">Brassica napus
      subsp. napobrassica</trd>) estas man&gcirc;eblaj;
      alian subspecion oleri&ccirc;an oni nomas oleonapo a&ubreve; <ref tip="sub" cel="kolz.0o">kolzo</ref>:
      <ekz>
        la&ubreve; E&ubreve;ropa Komisiono &ccirc;irka&ubreve; 10-15&nbsp;% de la agrikulturaj terenoj
        en E&ubreve;ropo povus esti dedi&ccirc;itaj al oleo<tld/>o, provizante &ccirc;irka&ubreve;
        20&nbsp;% de la brula&jcirc;o konsumata de la trafiko e&ubreve;ropa
        <fnt>
          <bib>Monato</bib>
          <aut>Pierre Bertrand</aut>
          <vrk><url ref="&Monato;/2006/008926.php">La fino
          de malmultekosta energio</url></vrk>
        </fnt>.
      </ekz>
    </dif>    
    <ref tip="vid" cel="bet.0o">beto</ref>
    <bld lok="&WCU;/thumb/a/a7/Rutabaga%2C_variety_nadmorska.JPG/640px-Rutabaga%2C_variety_nadmorska.JPG" prm="CC BY-SA 3.0"><tld/>otuberoj
        <fnt>
          <aut>Picasa user Seedambassadors,
          http://picasaweb.google.com/seedambassadors/SscVarieties#5296490359767135106</aut>
          <lok><url ref="&WCW;/File:Rutabaga,_variety_nadmorska.JPG">&WCW;/File:Rutabaga,_variety_nadmorska.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_u;&c_ch;&c_k;&c_a;</trd>
  <trdgrp lng="bg">
    <trd>&c_zh;&c_malmol;&c_l;&c_t;&c_a;
    <ind>&c_r;&c_ja;&c_p;&c_a;</ind></trd>,
    <trd>&c_b;&c_r;&c_ju;&c_k;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="br">irvinenn</trd>
  <trd lng="ca">nap</trd>
  <trdgrp lng="cs">
    <trd>&rcaron;epka olejka</trd>,
    <trd>tu&rcaron;&iacute;n</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Steckr&uuml;be</trd>,
    <trd>Kohlr&uuml;be</trd>
  </trdgrp>
  <trd lng="el">&gamma;&omikron;&gamma;&gamma;&ypsilon_ton;&lambda;&jota;</trd>
  <trdgrp lng="en">
    <trd>rape</trd>,
    <trd>rapeseed</trd>,
    <trd>canola</trd>
  </trdgrp>
  <trd lng="es">nabo</trd>
  <trd lng="fa">&a_sh;&a_l;&a_gh;&a_m;</trd>
  <trd lng="fr">navet</trd>
  <trd lng="hu">kar&oacute;r&eacute;pa</trd>
  <trdgrp lng="ja">
    <trd>&#x30AB;&#x30D6;</trd>,
    <trd>&#x30AB;&#x30D6;&#x30E9;</trd>,
    <trd>&#x30BB;&#x30A4;&#x30E8;&#x30A6;&#x30A2;&#x30D6;&#x30E9;&#x30CA;</trd>
  </trdgrp>
  <trd lng="nl">koolraap</trd>
  <trd lng="pl">kapusta rzepak</trd>
  <trd lng="pt">nabo</trd>
  <trd lng="ru">&c_b;&c_r;&c_ju;&c_k;&c_v;&c_a;</trd>
  <trd lng="sk">repka olejn&aacute;</trd>
  <trd lng="sv">k&aring;lrot</trd>
  <trd lng="uk">&c_b;&c_r;&c_u;&#x43A;&c_v;&c_a;</trd>
  <trd lng="zh">&#x82B8;&#x82D4;&#x5B50; <pr>y&#xFA;nt&#xE1;iz&#x1D0;</pr></trd>
</drv>

<drv mrk="nap.0okapa">
  <kap>
    <tld/>okapa
  </kap>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="stult.0a">Stulta</ref>, mense limigita:
      <ekz>
        <ind><tld/>okapa</ind>, amatora &jcirc;urnalisto fanatike deliranta
        <fnt>
          <aut>Gilbert Ledon</aut>
            <vrk><url ref="&Monato;/2000/007264.php">Morti miskomprene (1)</url></vrk>
            <bib>Monato</bib>
            <lok>2000/12, p. 4</lok>
          </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="sxtip.0o.malsagxulo">&scirc;tipo<sncref/></ref>
  </snc>
  <trd lng="cs">hloup&yacute;</trd>
  <trdgrp lng="de">
     <trd>engstirnig</trd>,
     <trd>beschr&auml;nkt <klr>(doof)</klr></trd>,
     <trd>bescheuert</trd>,
     <trd>bekloppt</trd>
  </trdgrp>
  <trd lng="en">dumb</trd>
  <trdgrp lng="zh">
    <trd>&#x591A;&#x5708; <pr>du&#x14D;ju&#xE0;n</pr></trd>,
    <trd>&#x8111;&#x5B50;&#x6709;&#x6CE1; <pr>n&#x1CE;oziy&#x1D2;up&#xE0;o</pr></trd>,
    <trd>&#x8166;&#x5B50;&#x6709;&#x6CE1; <pr>n&#x1CE;oziy&#x1D2;up&#xE0;o</pr></trd>
  </trdgrp>
</drv>



</art>
