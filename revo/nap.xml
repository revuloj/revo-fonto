<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>nap</rad>/o
</kap>
<drv mrk="nap.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="brasik.0o">brasiko</ref>
      (<trd lng="la">Brassica napus</trd>),
      kies tubera radiko estas man&gcirc;ebla.  
    </dif>
    <refgrp tip="sub">
      <ref cel="brasik.0napo">brasiknapo</ref>,
      <ref cel="kolz.0o">kolzo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Kohlr&uuml;be</trd>
</drv>
</art>
