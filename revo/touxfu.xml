<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>to&ubreve;fu</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="touxfu.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Pasto el koagulita lakto de sojfaboj premita kaj kutime distran&ccirc;ita
      en kubetojn.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_o;&c_f;&c_u;</trd>
  <trdgrp lng="de">
      <trd>Tofu</trd>,
      <trd>Sojabohnenpaste</trd>,
      <trd>Sojabrei</trd>
   </trdgrp>
  <trdgrp lng="fr">
    <trd>tofu</trd>
  </trdgrp>
  <trdgrp lng="hu">
      <trd>sz&oacute;jasajt</trd>,
      <trd>tofu</trd>
   </trdgrp>
  <trdgrp lng="nl">
    <trd>tofu</trd>
  </trdgrp>
  <trd lng="pt">tofu</trd>

</drv>
</art>

