<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>agres</rad>/i
</kap>
<drv mrk="agres.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ataki la unua, ne estante mem minacata.
    </dif>
  </snc>
  <trd lng="es">agredir</trd>
  <trd lng="fr">agresser</trd>
  <trd lng="nl">agressie plegen</trd>
  <trd lng="pt">agredir</trd>
</drv>

<drv mrk="agres.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <dif>
      &Ccirc;iam preta ataki, e&ccirc; ne provokite.
    </dif>
  </snc>
  <trd lng="es">agresivo</trd>
  <trd lng="fr">agressif</trd>
  <trd lng="nl">agressief</trd>
  <trd lng="sv">aggressiv</trd>
  <trd lng="pt">agressivo</trd>
  <trd lng="ru">&c_a;&c_g;&c_r;&c_je;&c_s;&c_s;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
