<art mrk="$Id: agres.xml,v 1.32 2023/10/12 16:57:43 revo Exp $">
<kap>
  <ofc>9</ofc>
  <rad>agres</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="agres.0i">
  <kap><ofc>9</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ataki ne provokite; ataki kiel unua ne estante mem minacata de
      atako:
      <ekz>
        <tld/>i personon, landon
        <fnt><bib>OA9</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_p;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_t;&c_a;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>angreifen</trd>,
    <trd>zuerst angreifen</trd>,
    <trd>&uuml;berfallen</trd>
  </trdgrp>
  <trd lng="en">aggress</trd>
  <trd lng="es">agredir</trd>
  <trd lng="fr">agresser</trd>
  <trd lng="he">&#x5DC;&#x5D4;&#x5D9;&#x5D5;&#x5EA; &#x5EA;&#x5D5;&#x5E7;&#x5E4;&#x5E0;&#x5D9;</trd>
  <trd lng="it">aggredire</trd>
  <trdgrp lng="ja">
    <trd>&#x6226;&#x3044;&#x3092;&#x3057;&#x304B;&#x3051;&#x308B;  <pr>&#x305F;&#x305F;&#x304B;&#x3044;&#x3092;&#x3057;&#x304B;&#x3051;&#x308B;</pr></trd>,
    <trd>&#x4FB5;&#x7565;&#x3059;&#x308B;  <pr>&#x3057;&#x3093;&#x308A;&#x3083;&#x304F;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">agressie plegen</trd>
  <trd lng="pt">agredir</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_p;&c_a;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_a;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_a;&c_t;&c_a;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">&uacute;to&ccaron;i&tcaron;</trd>
  <trdgrp lng="tr">
    <trd>sata&scedil;mak</trd>,
    <trd>sald&inodot;rmak</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x443;&#x447;&#x438;&#x43D;&#x438;&#x442;&#x438;/&#x437;&#x434;&#x456;&#x439;&#x441;&#x43D;&#x438;&#x442;&#x438; &#x430;&#x491;&#x440;&#x435;&#x441;&#x456;&#x44E;</trd>,
    <trd>&#x43D;&#x430;&#x43F;&#x430;&#x441;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="agres.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Spontanea, nedefenda subita atako:
      <ekz>
        procesoj kondamnis urbestrojn a&ubreve; kunlaborantojn
        &ccirc;efe pro
        agresoj,
        insultoj
        <fnt><aut>Adrien Durandard</aut>: <vrk>FN-fono</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 7a, p. 12a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_g;&c_r;&c_e;&c_s;&c_ib;&c_ja;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_a;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>agrese</trd>,
    <trd>p&rcaron;epad</trd>,
    <trd>v&yacute;boj</trd>,
    <trd>&uacute;tok</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Angriff</trd>,
    <trd>Attacke</trd>,
    <trd>&Uuml;berfall</trd>
  </trdgrp>
  <trd lng="en">aggression</trd>
  <trd lng="es">agresi&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>agression</trd>
  </trdgrp>
  <trd lng="he">&#x5EA;&#x5D5;&#x5B9;&#x5E7;&#x5E4;&#x5BC;&#x5B8;&#x5E0;&#x5D5;&#x5BC;&#x5EA;</trd>
  <trd lng="it">aggresione</trd>
  <trdgrp lng="ja">
    <trd>&#x4FB5;&#x7565;  <pr>&#x3057;&#x3093;&#x308A;&#x3083;&#x304F;</pr></trd>,
    <trd>&#x653B;&#x6483;  <pr>&#x3053;&#x3046;&#x3052;&#x304D;</pr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_g;&c_r;&c_je;&c_s;&c_s;&c_i;&c_ja;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_a;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>agresia</trd>,
    <trd>dobyva&ccaron;n&yacute; &uacute;tok</trd>,
    <trd>prepad</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>sald&inodot;r&inodot;</trd>,
    <trd>sata&scedil;ma</trd>
  </trdgrp>
  <trd lng="uk">&#x430;&#x491;&#x440;&#x435;&#x441;&#x456;&#x44F;</trd>
</drv>

<drv mrk="agres.0ema">
  <kap><ofc>9</ofc><tld/>ema <fnt><bib>SPV</bib></fnt></kap>
  <snc>
    <dif>
      &Ccirc;iam preta ataki, a&ubreve; inklina kutime ataki, e&ccirc;
      ne provokite:
      <ekz>
        <uzo tip="stl">FIG</uzo>pura, dika roksono, iom
        <tld/>ema
        <fnt><aut>Manuel Pancorbo Castro</aut>: <vrk>Al la
        barikadoj! (JoMo kaj Liberecanoj)</vrk>,
        <bib>Monato</bib><lok>jaro 1999a, numero 7a, p. 24a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="atak.0ema">atakema</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>angriffslustig</trd>,
    <trd>angriffig [CH]</trd>,
    <trd>aggressiv</trd>
  </trdgrp>
  <trd lng="be">&c_a;&c_g;&c_r;&c_e;&c_s;&c_ib;&c_w;&c_n;&c_y;</trd>
  <trd lng="en">aggressive</trd>
  <trd lng="es">agresivo</trd>
  <trd lng="fr">agressif</trd>
  <trd lng="he">&#x5EA;&#x5D5;&#x5B9;&#x5E7;&#x5E4;&#x5BC;&#x5B8;&#x5E0;&#x5B4;&#x5D9;</trd>
  <trd lng="it">aggresivo</trd>
  <trdgrp lng="ja">
    <trd>&#x653B;&#x6483;&#x7684;&#x306A;  <pr>&#x3053;&#x3046;&#x3052;&#x304D;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x597D;&#x6226;&#x7684;&#x306A;  <pr>&#x3053;&#x3046;&#x3044;&#x304F;&#x3055;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x4FB5;&#x7565;&#x7684;&#x306A;  <pr>&#x3057;&#x3093;&#x308A;&#x3083;&#x304F;&#x3066;&#x304D;&#x306A;</pr></trd>
  </trdgrp>
  <trd lng="nl">agressief</trd>
  <trd lng="sv">aggressiv</trd>
  <trd lng="pt">agressivo</trd>
  <trd lng="ms">agresif</trd>
  <trd lng="ru">&c_a;&c_g;&c_r;&c_je;&c_s;&c_s;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>&uacute;to&ccaron;n&yacute;</trd>,
    <trd>agres&iacute;vny</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>sata&scedil;kan</trd>,
    <trd>sald&inodot;rgan</trd>
  </trdgrp>
  <trd lng="uk">&#x430;&#x491;&#x440;&#x435;&#x441;&#x438;&#x432;&#x43D;&#x438;&#x439;</trd>
</drv>

<drv mrk="agres.0emo">
  <kap><tld/>emo</kap>
  <snc>
    <dif>
      Eco de iu <tld/>ema:
      <ekz>
        foje &scirc;i plendis pro la <tld/>emo de fifotistoj
        <fnt><aut>Paul Gubbins</aut>: <vrk>Diana... &ccirc;u
        diino?</vrk>,
        <bib>Monato</bib><lok>jaro 1997a, numero 9a, p. 16a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_g;&c_r;&c_e;&c_s;&c_ib;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="cs">agresivita</trd>
  <trdgrp lng="de">
    <trd>Angriffslust</trd>,
    <trd>Aggressivit&auml;t</trd>
  </trdgrp>
  <trd lng="es">agresividad</trd>
  <trdgrp lng="fr">
    <trd>agressivit&eacute;</trd>
  </trdgrp>
  <trd lng="he">&#x5EA;&#x5D5;&#x5B9;&#x5E7;&#x5E4;&#x5BC;&#x5B8;&#x5E0;&#x5D5;&#x5BC;&#x5EA;</trd>
  <trd lng="it">aggresivit&agrave;</trd>
  <trd lng="ja">&#x653B;&#x6483;&#x6027;  <pr>&#x3053;&#x3046;&#x3052;&#x304D;&#x305B;&#x3044;</pr></trd>
  <trd lng="ru">&c_a;&c_g;&c_r;&c_je;&c_s;&c_s;&c_i;&c_v;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">agresivita</trd>
  <trd lng="tr">sald&inodot;rganl&inodot;k</trd>
</drv>
</art>
