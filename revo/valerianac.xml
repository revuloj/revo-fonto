<vortaro>
<art mrk="$Id: valerianac.xml,v 1.10 2022/12/28 08:32:10 revo Exp $">
<kap>
  <rad>valerianac</rad>/oj
</kap>
<drv mrk="valerianac.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="valerianac.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref> de <ref
      tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>
      vegeta&jcirc;oj (<trd lng="la">Valerianaceae</trd>):
      <ekz>
        <ref tip="prt" cel="valerian.0o">valeriano</ref> estas genro el <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=50438">Valeriano (planto)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Baldriangew&auml;chse</trd>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_l;&c_ja;&c_r;&#8217;&c_ja;&c_n;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  <trd lng="ca">valerian&agrave;cies</trd>
  <trd lng="es">valerian&aacute;ceas</trd>
  <trd lng="fr">valerianac&eacute;es</trd>
  <trd lng="hu">macskagy&ouml;k&eacute;rf&eacute;l&eacute;k</trd>
  <trd lng="pl">koz&lstroke;kowate</trd>
  <trd lng="pt">valerian&aacute;ceas</trd>
  <trd lng="uk">&c_v;&c_a;&#x43B;&c_je;&c_r;&c_ib;&c_a;&#x43D;&#x43E;&c_v;&c_ib;</trd>
</drv>
</art>

