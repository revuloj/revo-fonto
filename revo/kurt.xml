<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: kurt.xml,v 1.8 2026/02/06 02:55:02 revo Exp $">
    <kap>
      <rad>kurt</rad>/a
    </kap>

<drv mrk="kurt.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">NEO</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="long.mal0a">Mallonga</ref>:
      <ekz>
        niaj tro <tld/>aj someroj
        <fnt>
          <bib>Monato</bib>
          <aut>Marcel Lemeltier</aut>
          <vrk><url ref="&Monato;/2012/010235.php">&Ccirc;u forgesita trezoro?</url></vrk>
          <lok>2012</lok>
        </fnt>!
      </ekz>
      <ekz>
        la <tld/>perspektivaj interesoj de partioj
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki LINS</aut>
          <vrk><url ref="&Monato;/2004/008365.html"
              >Civitanoj perdas intereson pri E&ubreve;ropo</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        vortoj estis <tld/>igitaj per strekoj: &leftquot;sankta&rightquot; &ndash; &leftquot;s-ta&rightquot;
        <fnt>
          <bib>Kon11</bib>
          <lok>Lingvoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la jupo tre <tld/>as, la kruroj tre belas, kaj mi fajfe spiretas
        <fnt>
          <bib>Monato</bib>
          <aut>Daniel L. Mason</aut>
          <vrk><url ref="&Monato;/2011/010276.php">Matena kafo</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekmovi&gcirc;is strioj de fluanta akvo: <tld/>aj kaj longaj, rapidaj kaj lantaj, gajaj
        kaj tristaj depende de la akompana melodio
        <fnt>
          <bib>Monato</bib>
          <aut>Tatjana Loskutova</aut>
          <vrk><url ref="&Monato;/2007/009164.php">Voja&gcirc;o en Iberio</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la randoj de la tombo&scirc;tono videblas orumitaj skriba&jcirc;oj el Korano kaj
        <tld/>aj instruaj sentencoj pri la agoj kaj eldiroj de profeto Mahometo
        <fnt>
          <bib>Monato</bib>
          <aut>Anatoli Ionesov</aut>
          <vrk><url ref="&Monato;/2006/008920.php">Perlo &ccirc;e la Silka Vojo</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">kurz</trd>
    <trd lng="en">short</trd>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>mallonga</ke>.
  </rim>

  <trd lng="be">&c_k;&c_a;&c_r;&c_o;&c_t;&c_k;&c_ib;</trd>
  <trd lng="pl">kr&oacute;tki</trd>
  <trd lng="uk">&c_k;&c_o;&c_r;&c_o;&c_t;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#36618; <pr>zh&igrave;</pr></trd>,
    <trd>&#36734; <pr>zh&igrave;</pr></trd>,
    <trd>&#20468; <pr>ji&agrave;n</pr></trd>,
    <trd>&#30696; <pr>y&#464;ng</pr></trd>
  </trdgrp>
</drv>

  </art>

