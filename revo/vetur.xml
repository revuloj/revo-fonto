<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>vetur</rad>/i
    </kap>

    <drv mrk="vetur.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
        <dif>
          <ref tip="super" cel="ir.0i">Iri</ref> ne pa&scirc;ante sed
          transportate per diversaj rimedoj:
          <ekz>
            morga&ubreve; mi <tld/>os Parizon<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>i per kale&scirc;o<fnt>B</fnt>,
          </ekz><ekz>
            per aviadilo, en vagonaro<fnt>B</fnt>,
          </ekz><ekz>
            sur &scirc;ipo<fnt>B</fnt>,
          </ekz><ekz>
            je &ccirc;evalo
            (<ref tip="sin" cel="rajd.0i">rajdi</ref>);
          </ekz><ekz>
            la princo permesis al li <tld/>i kun si kaj ili
            ekgalopis<fnt>B</fnt>;
          </ekz><ekz>
            aer<tld/>i, mar<tld/>i, &scirc;ip<tld/>i, vel<tld/>i
            <klr>(<tld/>i en aero, maro, per &scirc;ipo, velo)</klr>.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="krur.0o">kruro</ref>,
          <ref cel="pasagx.0o">pasa&gcirc;ero</ref>.
        </refgrp>
        <trdgrp lng="hu">
          <trd>megy <klr tip="ind">(j&aacute;rm&udblac;vel)</klr></trd>,
          <trd>utazik</trd>
        </trdgrp>
      </snc>
      <snc>
        <dif>
          Transloki&gcirc;i pp <tld/>iloj:
          <ekz>
            <tld/>as vagonaro<fnt>Z</fnt>,
          </ekz><ekz>
            &scirc;ipo<fnt>B</fnt>,
          </ekz><ekz>
            biciklo<fnt>B</fnt>,
          </ekz><ekz>
            fiakro.
          </ekz>
        </dif>
        <trd lng="hu">k&ouml;zlekedik</trd>
      </snc>
      <trd lng="be">&c_je;&c_h;&c_a;&c_c;&c_mol;</trd>
      <trd lng="cs">jet</trd>
      <trd lng="de">fahren</trd>
      <trd lng="en">ride</trd>
      <trdgrp lng="fr">
        <trd>aller<klr> (en v&eacute;hicule)</klr></trd>,
        <trd>se d&eacute;placer<klr> (en v&eacute;hicule)</klr></trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>reizen</trd>,
        <trd>rijden <klr tip="ind">(in een voertuig)</klr></trd>
      </trdgrp>
      <trd lng="pt">ir<klr tip="ind">(com alguma esp&eacute;cie de
          ve&iacute;culo)</klr></trd>
      <trdgrp lng="ru">
        <trd><ofc>*</ofc>&c_je;&c_h;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_je;&c_z;&c_d;&c_i;&c_t;&c_mol;</trd>
      </trdgrp>
      <trdgrp lng="sv">
        <trd>&aring;ka</trd>,
        <trd>fara</trd>,
        <trd>f&auml;rdas</trd>
      </trdgrp>
    </drv>

    <drv mrk="vetur.0igi">
      <kap><tld/>igi</kap>
      <snc>
        <dif>
          Transporti per diversaj rimedoj:
          <ekz>
            jen la lokomotivo, kiu nin <tld/>igos;
          </ekz><ekz>
            <tld/>igi grenon al bazaro<fnt>B</fnt>;
          </ekz><ekz>
            li metis la kadavron sur la azenon kaj <tld/>igis &gcirc;in
            returne<fnt>Z</fnt>.
          </ekz>
        </dif>
        <trd lng="be">&c_v;&c_je;&c_z;&c_mol;&c_c;&c_ib;</trd>
        <trdgrp lng="de">
          <trd>transportieren</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>transporter</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>sz&aacute;ll&iacute;t</trd>,
          <trd>fuvaroz</trd>
        </trdgrp>
        <trd lng="nl">vervoeren</trd>
        <trdgrp lng="ru">
          <trd>&c_v;&c_je;&c_z;&c_t;&c_i;</trd>,
          <trd>&c_v;&c_o;&c_z;&c_i;&c_t;&c_mol;</trd>
        </trdgrp>
        <trdgrp lng="sv">
          <trd>skjutsa</trd>,
          <trd>k&ouml;ra</trd>
        </trdgrp>
      </snc>
      <snc>
        <dif>
          Direkti <tld/>ilon:
          <ekz>
            malgranda kale&scirc;o, <tld/>igata de unu knabo<fnt>Z</fnt>.
          </ekz>
        </dif>
        <trdgrp lng="be">
          <trd>&c_k;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
          <trd>&c_v;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>
        </trdgrp>
        <trd lng="hu">vezet <klr tip="ind">(j&aacute;rm&udblac;vet)</klr></trd>
        <trd lng="nl">besturen</trd>
        <trdgrp lng="fr">
          <trd>conduire</trd>,
          <trd>mener<klr> (un v&eacute;hicule)</klr></trd>
        </trdgrp>
        <trd lng="ru">&c_v;&c_je;&c_s;&c_t;&c_i;<klr>
            (&c_m;&c_a;&c_sh;&c_i;&c_n;&c_u;, &c_k;&c_a;&c_r;&c_je;&c_t;&c_u; &c_i;
            &c_t;.&c_p;.)</klr></trd>
        <trd lng="sv">k&ouml;ra</trd>
      </snc>
      <trd lng="pt">transportar</trd>
    </drv>

    <drv mrk="vetur.0igisto">
      <kap><tld/>igisto</kap>
      <snc mrk="vetur.0igisto.TRA">
        <dif>
          Homo, kiu direktas <tld/>ilon:
          <ekz>
            la <tld/>igisto neniam ion senbezonan eldiris
            <fnt><bib>Ne&gcirc;aBlovado</bib></fnt>.
          </ekz>
        </dif>
        <refgrp tip="sub">
          <ref cel="auxt.0isto">a&ubreve;tisto</ref>,
          <ref cel="kocxer.0o">ko&ccirc;ero</ref>,
          <ref cel="pilot.0o">piloto</ref>,
          <ref cel="sxofor.0o">&scirc;oforo</ref>
        </refgrp>
      </snc>
      <trdgrp lng="be">
        <trd>&c_k;&c_ib;&c_r;&c_o;&c_w;&c_c;&c_a;</trd>,
        <trd>&c_v;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_je;&c_l;&c_mol;</trd>,
        <trd>&c_v;&c_o;&c_z;&c_ch;&c_y;&c_k;</trd>,
        <trd>&c_f;&c_u;&c_r;&c_m;&c_a;&c_n;</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Fahrer</trd>,
      </trdgrp>
      <trdgrp lng="fr">
        <trd>chauffeur</trd>,
        <trd>conducteur</trd>,
        <trd>pilote</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>(j&aacute;rm&udblac;)<ind>vezet&odblac;</ind></trd>,
        <trd>kocsis</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_v;&c_o;&c_z;&c_n;&c_i;&c_c;&c_a;</trd>,
        <trd>&c_k;&c_u;&c_ch;&c_je;&c_r;</trd>,
        <trd>&c_ja;&c_m;&c_shch;&c_i;&c_k;</trd>,
        <trd>&c_i;&c_z;&c_v;&c_o;&c_z;&c_ch;&c_i;&c_k;</trd>,
        <trd>&c_v;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
      </trdgrp>
      <trdgrp lng="sv">
        <trd>f&ouml;rare</trd>,
        <trd>chauff&ouml;r</trd>
      </trdgrp>
    </drv>

    <drv mrk="vetur.0ilo">
      <kap><ofc>*</ofc><tld/>ilo</kap>
      <snc mrk="vetur.0ilo.TRA">
        <tezrad fak="TRA"/>	  
        <uzo tip="fak">TRA</uzo>
        <dif>
          Ilo por transportado, konsistanta esence el iaforma kesto,
          kaj plejofte radoj a&ubreve; glitiloj:
          <ekz>
            en somero ni <tld/>as per diversaj <tld/>iloj
            <fnt><bib>F</bib>, <lok>&FE; 34</lok></fnt>;
          </ekz><ekz>
            na&gcirc;<tld/>ilo<fnt>Z</fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_a;&c_r;&c_t;</trd>,
        <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_a;&c_r;&c_t;&c_n;&c_y;
          &c_s;&c_r;&c_o;&c_d;&c_a;&c_k;</trd>,
        <trd>&c_k;&c_a;&c_l;&c_jo;&c_s;&c_y;</trd>,
        <trd>&c_p;&c_a;&c_v;&c_o;&c_z;&c_k;&c_a;</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Fahrzeug</trd>,
      </trdgrp>
      <trdgrp lng="fr">
        <trd>moyen de <ind>transport</ind></trd>,
        <trd>v&eacute;hicule</trd>
      </trdgrp>
      <trd lng="hu">j&aacute;rm&udblac;</trd>
      <trd lng="nl">voertuig</trd>
      <trdgrp lng="ru">
        <trd>&c_s;&c_r;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o;
          &c_p;&c_je;&c_r;&c_je;&c_d;&c_v;&c_i;&c_zh;&c_je;&c_n;&c_i;&c_ja;</trd>,
        <trd>&c_e;&c_k;&c_i;&c_p;&c_a;&c_zh;</trd>,
        <trd>&c_p;&c_o;&c_v;&c_o;&c_z;&c_k;&c_a;</trd>,
        <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_o;&c_r;&c_t;</trd>
      </trdgrp>
      <trdgrp lng="zh">
        <trd>&#36710;</trd>,
        <trd><klr>tr. </klr>&#36554;<klr> [ch&#275;]</klr></trd>
      </trdgrp>
      <trd lng="sv">fordon</trd>
    </drv>

    <drv mrk="vetur.cxirkaux0i">
      <kap>&ccirc;irka&ubreve;<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="vetur.cxirkaux0i.kun_objekto">
        <dif>
          <tld lit="V"/>i &ccirc;irka&ubreve;:
          <ekz>
            la glitveturilo dufoje &ccirc;irka&ubreve;<tld/>is la placon
            <fnt><bib>Fab2</bib>, <lok>Ne&gcirc;a re&gcirc;ino</lok></fnt>.
          </ekz>
        </dif>
        <trdgrp lng="hu">
          <trd>k&ouml;rbeutazik</trd>,
          <trd>megker&uuml;l <klr tip="ind">(j&aacute;rm&udblac;vel)</klr></trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_o;&c_b;&c_malmol;&c_je;&c_h;&c_a;&c_t;&c_mol;</trd>,
          <trd>&c_o;&c_b;&c_malmol;&c_je;&c_z;&c_zh;&c_a;&c_t;&c_mol;</trd>
        </trdgrp>
      </snc>
      <snc mrk="vetur.cxirkaux0i.absolute">
        <uzo tip="klr">(abs.)</uzo>
        <dif>
          <tld lit="V"/>i en la &ccirc;irka&ubreve;a&jcirc;o:
          <ekz>
            <klr>[la veturilon]</klr> tiris du flavaj bovoj, kun kiuj Georgo
            havis la permeson iom &ccirc;irka&ubreve;<tld/>i
            <fnt><bib>Fab4</bib>, <lok>Historio el la dunoj</lok></fnt>.
          </ekz>
        </dif>
        <trd lng="hu">ker&uuml;l egyet <klr tip="ind">(j&aacute;rm&udblac;vel)</klr></trd>
        <trd lng="ru">&c_r;&c_a;&c_z;&c_malmol;&c_je;&c_z;&c_zh;&c_a;&c_t;&c_mol;
&c_k;&c_r;&c_u;&c_g;&c_o;&c_m;</trd>
      </snc>
    </drv>

    <drv mrk="vetur.el0i">
      <kap>el<tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc mrk="vetur.el0i.KOMUNE">
        <dif>
          Eliri <tld/>ante:
          <ekz>
            li &jcirc;us el<tld/>is hiera&ubreve;.
          </ekz>
        </dif>
        <trdgrp lng="be">
          <trd>&c_z;&c_mol;&c_je;&c_h;&c_a;&c_c;&c_mol;</trd>,
          <trd>&c_v;&c_y;&c_je;&c_h;&c_a;&c_c;&c_mol;</trd>
        </trdgrp>
        <trdgrp lng="de">
          <trd>ausfahren</trd>,
          <trd>herausfahren</trd>,
        </trdgrp>
        <trdgrp lng="fr">
          <trd>sortir<klr> (en v&eacute;hicule)</klr></trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>kiutazik</trd>,
          <trd>kihajt <klr tip="ind">(j&aacute;rm&udblac;vel)</klr></trd>,
          <trd>kij&aacute;r <klr tip="ind">(j&aacute;rm&udblac;vel)</klr></trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_u;&c_je;&c_h;&c_a;&c_t;&c_mol;</trd>,
          <trd>&c_v;&c_y;&c_je;&c_h;&c_a;&c_t;&c_mol;</trd>
        </trdgrp>
      </snc>
    </drv>

    <drv mrk="vetur.en0i">
      <kap>en<tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc mrk="vetur.en0i.KOMUNE">
        <dif>
          Eniri <tld/>ante:
          <ekz>
            la kale&scirc;o en<tld/>is en la korton<fnt>B</fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="de">
        <trd>einfahren</trd>,
        <trd>hineinfahren</trd>,
      </trdgrp>
      <trdgrp lng="be">
        <trd>&c_u;&c_je;&c_h;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_z;&c_a;&c_je;&c_h;&c_a;&c_c;&c_mol;</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>entrer<klr> (avec un v&eacute;hicule)</klr></trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>beutazik</trd>,
        <trd>behajt <klr tip="ind">(j&aacute;rm&udblac;vel)</klr></trd>,
        <trd>bej&aacute;r <klr tip="ind">(j&aacute;rm&udblac;vel)</klr></trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_v;&c_malmol;&c_je;&c_h;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_z;&c_a;&c_je;&c_h;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
    </drv>

    <drv mrk="vetur.glit0ilo">
      <kap><ofc>*</ofc>glit<tld/>ilo</kap>
      <snc mrk="vetur.glit0ilo.TRA">
        <uzo tip="fak">TRA</uzo>
        <dif>
          <ref tip="super" cel="vetur.0ilo.TRA"><tld lit="V"/>ilo</ref> kiu
          staras sur glitiloj por iri sur ne&gcirc;o, glacio, eventuale akvo:
          <ekz>
            en vintro ni veturas per glit<tld/>ilo
            <fnt><bib>F</bib>, <lok>&FE; 34</lok></fnt>;
          </ekz><ekz>
            por si li ordonis prepari malgrandan glit<tld/>ilon unu&ccirc;evalan
            <fnt><bib>Ne&gcirc;aBlovado</bib></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="de">Schneemobil</trd>
      <trd lng="fr">traineau<klr tip="amb"> (v&eacute;hicule sur patins)</klr></trd>
      <trd lng="ru"><ofc>*</ofc>&c_s;&c_a;&c_n;&c_i;</trd>
    </drv>

    <drv mrk="vetur.pet0i">
      <kap>pet<tld/>i</kap>
      <snc mrk="vetur.pet0i.TRA">
        <uzo tip="fak">TRA</uzo>
        <dif>
          Voja&gcirc;i en veturilo, kies kondukanton oni haltigis sur la vojo
          kaj petis pri transportado:
          <ekz>
            mi ser&ccirc;as amikojn kiuj pet<tld/>adas &ccirc;irka&ubreve; la mondo, &scirc;atus
            kunveturi kaj havas sperton pri tio
            <fnt>
              <aut>A. Makarov</aut>,
              <vrk>15 julio 2004, Esperanto Koresponda Servo</vrk>,
              <lok><url
 ref="http://groups.google.be/group/soc.culture.esperanto?hl=eo"
                  >soc.culture.esperanto</url>,
                2004-07-15</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_p;&c_a;&c_d;&c_a;&c_r;&c_o;&c_zh;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;
<ind>&c_a;&c_w;&c_t;&c_a;&c_s;&c_t;&c_o;&c_p;&c_a;&c_m;</ind></trd>
      <trd lng="br">c'hoari <ind>biz-meud</ind></trd>
      <trdgrp lng="de">
        <trd>trampen</trd>,
        <trd>Autostop fahren</trd>,
        <trd>per Anhalter fahren</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>voyager en <ind>auto-stop</ind></trd>,
        <trd>voyager en <ind>stop</ind></trd>
      </trdgrp>
      <trd lng="hu">aut&oacute;stopozik</trd>
      <trdgrp lng="nl">
        <trd>liften</trd>
      </trdgrp>
      <trd lng="ru">&c_p;&c_u;&c_t;&c_je;&c_sh;&c_je;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;
&c_a;&c_v;&c_t;&c_o;&c_s;&c_t;&c_o;&c_p;&c_o;&c_m;</trd>
    </drv>

    <drv mrk="vetur.post0ilo">
      <kap>post<tld/>ilo</kap>
      <fnt><bib>PIV1</bib></fnt>
      <snc mrk="vetur.post0ilo.TRA">
        <uzo tip="fak">TRA</uzo>
        <dif>
          <tld lit="V"/>ilo trenata de alia <tld/>ilo:
          <ekz>
            ni starigu la tendon kaj malplenigu la a&ubreve;ton
            kaj la post<tld/>ilon
            <fnt>
              <aut>Ch. Bertin</aut>
              <vrk>L'esp&eacute;ranto avec plaisir</vrk>
              <lok><url ref="http://www.ifrance.fr/bertin/catal/eap.htm">
                  http://www.ifrance.fr/bertin/catal/eap.htm</url></lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="fr">remorque</trd>
      <trdgrp lng="hu">
        <trd>ut&aacute;nfut&oacute;</trd>,
        <trd>p&oacute;tkocsi</trd>
      </trdgrp>
      <trd lng="ru">&c_p;&c_r;&c_i;&c_c;&c_je;&c_p;</trd>
      <trd lng="sv">sl&auml;p</trd>
    </drv>

    <drv mrk="vetur.re0i">
      <kap>re<tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc mrk="vetur.re0i.KOMUNE">
        <dif>
          Reveni <tld/>ante.
        </dif>
      </snc>
      <trdgrp lng="fr">
        <trd>revenir<klr> (avec un v&eacute;hicule)</klr></trd>
      </trdgrp>
      <trd lng="be">&c_v;&c_ja;&c_r;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>
      <trd lng="hu">visszautazik</trd>
      <trd lng="ru">&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>
      <trd lng="sv">&aring;ka tillbaka</trd>
    </drv>

    <drv mrk="vetur.tra0i">
      <kap>tra<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="vetur.tra0i.KOMUNE">
        <dif>
          Trairi <tld/>ante:
          <ekz>
            tra<tld/>i la tutan E&ubreve;ropon<fnt>B</fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_p;&c_r;&c_a;&c_je;&c_h;&c_a;&c_c;&c_mol;<klr>
          (&c_s;&c_k;&c_r;&c_o;&c_z;&c_mol; &c_n;&c_je;&c_sh;&c_t;&c_a;,
&c_c;&c_a;&c_l;&c_k;&c_a;&c_m;)</klr></trd>
      <trdgrp lng="fr">
        <trd>passer par<klr> (en v&eacute;hicule)</klr></trd>,
        <trd>parcourir<klr> (en v&eacute;hicule)</klr></trd>
      </trdgrp>
      <trd lng="hu">&aacute;tutazik <klr tip="ind">(vmin)</klr></trd>
      <trd lng="ru">&c_p;&c_r;&c_o;&c_je;&c_h;&c_a;&c_t;&c_mol;<klr>
