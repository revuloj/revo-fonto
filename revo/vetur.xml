<kap>
  <ofc>*</ofc>
  <rad>vetur</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="vetur.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="ir.0i">Iri</ref> ne pa&scirc;ante sed
      transportate per diversaj rimedoj:
      <ekz>
        morga&ubreve; mi <tld/>os Parizon<fnt>Z</fnt>;
      </ekz><ekz>
        <tld/>i per kale&scirc;o<fnt>B</fnt>,
      </ekz><ekz>
        per aviadilo, en vagonaro<fnt>B</fnt>,
      </ekz><ekz>
        sur &scirc;ipo<fnt>B</fnt>,
      </ekz><ekz>
        je &ccirc;evalo
        (<ref tip="sin" cel="rajd.0i">rajdi</ref>);
      </ekz><ekz>
        la princo permesis al li <tld/>i kun si kaj ili
        ekgalopis<fnt>B</fnt>;
      </ekz><ekz>
        aer<tld/>i, mar<tld/>i, &scirc;ip<tld/>i, vel<tld/>i
        <klr>(<tld/>i en aero, maro, per &scirc;ipo, velo)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="krur.0o">kruro</ref>,
      <ref cel="pasagx.0o">pasa&gcirc;ero</ref>.
    </refgrp>
    <trdgrp lng="hu">
      <trd>megy <klr tip="ind">(j&aacute;rm&udblac;vel)</klr></trd>,
      <trd>utazik</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Transloki&gcirc;i pp <tld/>iloj:
      <ekz>
        <tld/>as vagonaro<fnt>Z</fnt>,
      </ekz><ekz>
        &scirc;ipo<fnt>B</fnt>,
      </ekz><ekz>
        biciklo<fnt>B</fnt>,
      </ekz><ekz>
        fiakro.
      </ekz>
    </dif>
    <trd lng="hu">k&ouml;zlekedik</trd>
  </snc>
  <trd lng="be">&c_je;&c_h;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bo">&#xF58;&#xF7C;&#xF0B;&#xF4A;&#xF0B;&#xF42;&#xF4F;&#xF7C;&#xF44;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>jet</trd>,
    <trd>v&eacute;zt se <klr>(&ccaron;&iacute;m, kam)</klr></trd>
  </trdgrp>
  <trd lng="de">fahren</trd>
  <trdgrp lng="en">
    <trd>travel<klr tip="amb"> (by vehicle)</klr></trd>,
    <trd>ride</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>ir<klr> (en un veh&iacute;culo)</klr></trd>,
    <trd>desplazarse<klr> (en un veh&iacute;culo)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>aller<klr> (en v&eacute;hicule)</klr></trd>,
    <trd>se d&eacute;placer<klr> (en v&eacute;hicule)</klr></trd>
  </trdgrp>
  <trd lng="id">ber<ind>kendara</ind></trd>
  <trd lng="io">vehar</trd>
  <trd lng="ja"><klr tip="amb">&#xFF08;&#x4E57;&#x308A;&#x7269;&#x3067;&#xFF09;</klr>&#x884C;&#x304F;
  [<ind>&#x3044;&#x304F;</ind>]</trd>
  <trd lng="ne">&#x921;&#x94D;&#x930;&#x93E;&#x907;&#x935;</trd>
  <trdgrp lng="nl">
    <trd>reizen<klr tip="amb"> (in een voertuig)</klr></trd>,
    <trd>rijden<klr tip="ind"> (in een voertuig)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>je&zacute;dzi&cacute;</trd>,
    <trd>jecha&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>andar de ve&iacute;culo</trd>,
    <trd>ir<klr tip="ind">(com alguma esp&eacute;cie de
      ve&iacute;culo)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd><ofc>*</ofc>&c_je;&c_h;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_je;&c_z;&c_d;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>cestova&tcaron;</trd>,
    <trd>chodi&tcaron;</trd>,
    <trd>viez&tcaron; sa</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>&aring;ka</trd>,
    <trd>fara</trd>,
    <trd>f&auml;rdas</trd>
  </trdgrp>
  <trd lng="tp">tawa<klr> (kepeken tomo tawa)</klr></trd>
  <trd lng="tr">arabayla bir yerden bir yere gitmek</trd>
</drv>

<drv mrk="vetur.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Transporti per diversaj rimedoj:
      <ekz>
        jen la lokomotivo, kiu nin <tld/>igos;
      </ekz><ekz>
        <tld/>igi grenon al bazaro<fnt>B</fnt>;
      </ekz><ekz>
        Kaj la profeto levis la kadavron de la homo de Dio kaj metis
        &gcirc;in sur la azenon kaj <tld/>igis &gcirc;in returne
       <fnt><bib>MT</bib>, <lok>&Re1; 13:29</lok></fnt>;
      </ekz><ekz> 
        Kiam nokte li <tld/>gis virinojn, li kutime atendis
        anta&ubreve; la enirejo &gcirc;is ili eniros en la lo&gcirc;ejon, por ke iu
        ulo ne embusku ilin.
        <fnt><aut>Julian Modest</aut>: <vrk>En la taksio.</vrk>
          <bib>LOdE</bib>, <lok>2001. &#x2116;11.</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_je;&c_z;&c_mol;&c_c;&c_ib;</trd>
    <trdgrp lng="de">
      <trd>transportieren</trd>
    </trdgrp>
    <trd lng="en">to transport</trd>
    <trdgrp lng="es">
      <trd>transportar</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>transporter</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>sz&aacute;ll&iacute;t</trd>,
      <trd>fuvaroz</trd>
    </trdgrp>
    <trd lng="io">vehigar</trd>
    <trd lng="ja"><klr tip="amb">&#xFF08;&#x4E57;&#x308A;&#x7269;&#x3067;&#xFF09;</klr>&#x904B;&#x3076;
      [<ind>&#x306F;&#x3053;&#x3076;</ind>]</trd>
    <trd lng="nl">vervoeren</trd>
    <trd lng="pl">wozi&cacute;</trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_je;&c_z;&c_t;&c_i;</trd>,
      <trd>&c_v;&c_o;&c_z;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>skjutsa</trd>,
      <trd>k&ouml;ra</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Direkti <tld/>ilon:
      <ekz>
        malgranda kale&scirc;o, <tld/>igata de unu
        knabo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_v;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="en">to drive</trd>
    <trdgrp lng="es">
      <trd>conducir</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>conduire</trd>,
      <trd>mener<klr> (un v&eacute;hicule)</klr></trd>
    </trdgrp>
    <trd lng="hu">vezet<klr tip="ind">
      (j&aacute;rm&udblac;vet)</klr></trd>
    <trd lng="ja"><klr>&#xFF08;&#x4E57;&#x308A;&#x7269;&#x3092;&#xFF09;</klr>&#x904B;&#x8EE2;&#x3059;&#x308B;
      [<ind>&#x3046;&#x3093;&#x3066;&#x3093;&#x3059;&#x308B;</ind>]</trd>
    <trd lng="nl">besturen</trd>
    <trdgrp lng="pl">
      <trd>prowadzi&cacute;</trd>,
      <trd>kierowa&cacute;</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_je;&c_s;&c_t;&c_i;<klr>
      (&c_m;&c_a;&c_sh;&c_i;&c_n;&c_u;, &c_k;&c_a;&c_r;&c_je;&c_t;&c_u; &c_i; &c_t;.&c_p;.)</klr></trd>
    <trd lng="sv">k&ouml;ra</trd>
    <trd lng="tr">ara&ccedil; s&uuml;rmek</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>vozit</trd>,
    <trd>v&eacute;zt</trd>,
    <trd>v&eacute;zt</trd>,
    <trd>&rcaron;&iacute;dit</trd>
  </trdgrp>
  <trd lng="id"><baz>kendara</baz>mengendarai</trd>
  <trd lng="pt">transportar</trd>
  <trdgrp lng="sk">
    <trd>riadi&tcaron;</trd>,
    <trd>viez&tcaron;</trd>,
    <trd>vozi&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="vetur.0igisto">
  <kap><tld/>igisto</kap>
  <snc mrk="vetur.0igisto.TRA">
    <dif>
      Homo, kiu direktas <tld/>ilon:
      <ekz>
        la <tld/>igisto neniam ion senbezonan eldiris
        <fnt><bib>Ne&gcirc;aBlovado</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="auxt.0isto">a&ubreve;tisto</ref>,
      <ref cel="kocxer.0o">ko&ccirc;ero</ref>,
      <ref cel="pilot.0o">piloto</ref>,
      <ref cel="sxofor.0o">&scirc;oforo</ref>
    </refgrp>
  </snc>
<trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_r;&c_o;&c_w;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_v;&c_o;&c_z;&c_ch;&c_y;&c_k;</trd>,
    <trd>&c_f;&c_u;&c_r;&c_m;&c_a;&c_n;</trd>
  </trdgrp>
<trdgrp lng="cs">
<trd>forman</trd>,
<trd>ko&ccaron;&iacute;</trd>,
<trd>vozka</trd>
</trdgrp>
<trdgrp lng="de">
    <trd>Fahrer</trd>,
  </trdgrp>
  <trd lng="en">driver</trd>
<trdgrp lng="es">
    <trd>conductor</trd>,
    <trd>maquinista</trd>,
    <trd>piloto</trd>
  </trdgrp>
<trdgrp lng="fr">
    <trd>chauffeur</trd>,
    <trd>conducteur</trd>,
    <trd>pilote</trd>
  </trdgrp>
<trdgrp lng="hu">
    <trd>(j&aacute;rm&udblac;)<ind>vezet&odblac;</ind></trd>,
    <trd>kocsis</trd>
  </trdgrp>
<trdgrp lng="pl">
    <trd>kierowca</trd>,
    <trd>przewo&zacute;nik</trd>,
    <trd>wo&zacute;nica<klr>
        (tylko dot. powoz&oacute;w konnych)</klr></trd>,
    <trd>powo&zdot;&aogonek;cy<klr>
        (tylko dot. powoz&oacute;w konnych)</klr></trd>,
    <trd>furman<klr> (tylko dot. powoz&oacute;w
        konnych)</klr></trd>,
    <trd>fornal<klr> (tylko dot. powoz&oacute;w
        konnych)</klr></trd>,
    <trd>szofer<klr> (tylko dot. samochod&oacute;w i
        autobus&oacute;w)</klr></trd>,
    <trd>motorniczy<klr>
        (tylko dot. tramwaj&oacute;w i trolejbus&oacute;w)</klr></trd>
  </trdgrp>
<trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_z;&c_n;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_k;&c_u;&c_ch;&c_je;&c_r;</trd>,
    <trd>&c_ja;&c_m;&c_shch;&c_i;&c_k;</trd>,
    <trd>&c_i;&c_z;&c_v;&c_o;&c_z;&c_ch;&c_i;&c_k;</trd>,
    <trd>&c_v;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="id"><baz>kendara</baz>pengendara</trd>
  <trd lng="id"><baz>kemudi</baz>pengemudi</trd>
<trd lng="sk">povozn&iacute;k</trd>
<trdgrp lng="sv">
    <trd>f&ouml;rare</trd>,
    <trd>chauff&ouml;r</trd>
  </trdgrp>
<trdgrp lng="tr">
    <trd>s&uuml;r&uuml;c&uuml;</trd>,
    <trd>&scedil;of&ouml;r</trd>
  </trdgrp>
</drv>

<drv mrk="vetur.0ilo">
  <kap><ofc>*</ofc><tld/>ilo</kap>
  <snc mrk="vetur.0ilo.TRA">
    <tezrad fak="TRA"/>
    <uzo tip="fak">TRA</uzo>
    <dif>
      Ilo por transportado, konsistanta esence el iaforma kesto,
      kaj plejofte radoj a&ubreve; glitiloj:
      <ekz>
        en somero ni <tld/>as per diversaj <tld/>iloj
        <fnt><bib>F</bib>, <lok>&FE; 34</lok></fnt>;
      </ekz><ekz>
        na&gcirc;<tld/>ilo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <lstref lst="voko:veturiloj">veturiloj</lstref>
  </snc>
  <trdgrp lng="be">
      <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_a;&c_r;&c_t;</trd>,
      <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_a;&c_r;&c_t;&c_n;&c_y;
        &c_s;&c_r;&c_o;&c_d;&c_a;&c_k;</trd>,
      <trd>&c_k;&c_a;&c_l;&c_jo;&c_s;&c_y;</trd>,
      <trd>&c_p;&c_a;&c_v;&c_o;&c_z;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&t_tha;&t_drengbu;&t_ga;&t_tsheg;&t_pa;&t_tsheg;</trd>  
  <trdgrp lng="cs">
  <trd>dopravn&iacute; prost&rcaron;edek</trd>,
  <trd>povoz</trd>,
  <trd>vozidlo</trd>,
  <trd>v&uring;z</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Fahrzeug</trd>,
    </trdgrp>
    <trd lng="en">vehicle</trd>
  <trdgrp lng="es">
      <trd>medio de <ind>transporte</ind></trd>,
      <trd>veh&iacute;culo</trd>
    </trdgrp>
  <trdgrp lng="fr">
      <trd>moyen de <ind>transport</ind></trd>,
      <trd>v&eacute;hicule</trd>
    </trdgrp>
  <trd lng="hu">j&aacute;rm&udblac;</trd>
  <trd lng="id"><ind>kendara</ind>an</trd>
  <trd lng="io">vehilo</trd>
  <trd lng="ja">&#x4E57;&#x308A;&#x7269; [<ind>&#x306E;&#x308A;&#x3082;&#x306E;</ind>]</trd>
  <trd lng="nl">voertuig</trd>
  <trdgrp lng="pl">
      <trd>pojazd</trd>,
      <trd>w&oacute;z</trd>,
      <trd>pow&oacute;z</trd>,
      <trd>fura<klr> (tak&zdot;e pot.)</klr></trd>
    </trdgrp>
  <trdgrp lng="ru">
      <trd>&c_s;&c_r;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o;
        &c_p;&c_je;&c_r;&c_je;&c_d;&c_v;&c_i;&c_zh;&c_je;&c_n;&c_i;&c_ja;</trd>,
      <trd>&c_e;&c_k;&c_i;&c_p;&c_a;&c_zh;</trd>,
      <trd>&c_p;&c_o;&c_v;&c_o;&c_z;&c_k;&c_a;</trd>,
      <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_o;&c_r;&c_t;</trd>
    </trdgrp>
  <trdgrp lng="sk">
  <trd>dopravn&yacute; prostriedok</trd>,
  <trd>povoz</trd>,
  <trd>vozidlo</trd>
  </trdgrp>
  <trd lng="sv">fordon</trd>
  <trd lng="tr">ta&scedil;&inodot;ma arac&inodot;</trd>
  <trdgrp lng="tp">
    <trd>tomo tawa<klr> (kun fermita ena spaco)</klr></trd>,
    <trd><klr>(malofte) </klr>ilo tawa</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8F66;</trd>,
    <trd><klr>tr. </klr>&#x8ECA; [<ind>ch&emacron;</ind>]</trd>
  </trdgrp>
</drv>

<drv mrk="vetur.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vetur.cxirkaux0i.kun_objekto">
    <dif>
      <tld lit="V"/>i &ccirc;irka&ubreve;:
      <ekz>
        la glitveturilo dufoje &ccirc;irka&ubreve;<tld/>is la placon
        <fnt><bib>Fab2</bib>, <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">to circle <klr>(by vehicle)</klr></trd>
    <trdgrp lng="hu">
      <trd>k&ouml;rbeutazik</trd>,
      <trd>megker&uuml;l
        <klr tip="ind">(j&aacute;rm&udblac;vel)</klr></trd>
    </trdgrp>
    <trd lng="pl">obje&zdot;d&zdot;a&cacute;</trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_malmol;&c_je;&c_h;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_malmol;&c_je;&c_z;&c_zh;&c_a;&c_t;&c_mol;
      </trd>
    </trdgrp>
  </snc>
  <snc mrk="vetur.cxirkaux0i.absolute">
    <uzo tip="klr">(abs.)</uzo>
    <dif>
      <tld lit="V"/>i en la &ccirc;irka&ubreve;a&jcirc;o:
      <ekz>
        <klr>[la veturilon]</klr> tiris du flavaj bovoj, kun kiuj
        Georgo havis la permeson iom &ccirc;irka&ubreve;<tld/>i
      <fnt><bib>Fab4</bib>, <lok>Historio el la dunoj</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="en">to drive around</trd>
    <trd lng="hu">ker&uuml;l egyet
      <klr tip="ind">(j&aacute;rm&udblac;vel)</klr></trd>
    <trdgrp lng="pl">
      <trd>przeje&zdot;d&zdot;a&cacute; si&eogonek;</trd>,
      <trd>karn&aogonek;&cacute; si&eogonek;<klr> (pot.)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_malmol;&c_je;&c_z;&c_zh;&c_a;&c_t;&c_mol; &c_k;&c_r;&c_u;&c_g;&c_o;&c_m;</trd>
  </snc>
</drv>

<drv mrk="vetur.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="vetur.0ejo.sxoseo">
    <dif>
      Tiu parto de placo, de urba strato a&ubreve; alia vojo, kiu estas
      destinita por <tld/>ado (kontraste al la
      <ref cel="trotua.0o">trotuaroj</ref>, gazonoj ktp):
      <ekz>
        same kiel aliloke, sur stratoj inter <tld/>ejo kaj
        piedirejo estas duonmetra akvofluejo, rande de kiu estas
        la betonbloka <ctl>barilo</ctl>
        <fnt>
          <aut>M. Lahtinen</aut>:
          <vrk>Kvar semajnoj en Armenio kaj Irano</vrk>
          <lok><url ref="http://users.jyu.fi/~mlahtine/ArIr2002/TTT-Unic/tehr.htm">Tehrano</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        fumo levi&gcirc;as el iu vendejo, kaj la fajrobrigado &jcirc;etas
        malsekajn, fulgajn meblojn sur la <tld/>ejon
        <fnt>
          <aut>Jenn&odblac; Rejt&odblac;, trad. L&aacute;szl&oacute; Bal&aacute;zs</aut>:
          <vrk><url ref="http://www.elerno.cn/bbs/dispbbs.asp?boardid=21&amp;id=5233">Pariza fronto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i eksaltis de la <tld/>ejo sur la trotuaron, provis
        rifu&gcirc;i en la enirejo, sed la elfluanta publiko baris al
        &scirc;i la vojon
