<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>vetur</rad>/i <fnt><bib>PV</bib></fnt>
    </kap>

    <drv mrk="vetur.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
        <dif>
          <ref tip="super" cel="ir.0i">Iri</ref> ne pa&scirc;ante sed
          transportate per diversaj rimedoj:
          <ekz>
            morga&ubreve; mi <tld/>os Parizon<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>i per kale&scirc;o<fnt>B</fnt>,
          </ekz><ekz>
            per aviadilo, en vagonaro<fnt>B</fnt>,
          </ekz><ekz>
            sur &scirc;ipo<fnt>B</fnt>,
          </ekz><ekz>
            je &ccirc;evalo
            (<ref tip="sin" cel="rajd.0i">rajdi</ref>);
          </ekz><ekz>
            la princo permesis al li <tld/>i kun si kaj ili
            ekgalopis<fnt>B</fnt>;
          </ekz><ekz>
            aer<tld/>i, mar<tld/>i, &scirc;ip<tld/>i, vel<tld/>i
            <klr>(<tld/>i en aero, maro, per &scirc;ipo, velo)</klr>.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="krur.0o">kruro</ref>,
          <ref cel="pasagx.0o">pasa&gcirc;ero</ref>.
        </refgrp>
        <trdgrp lng="hu">
          <trd>megy <klr tip="ind">(j&aacute;rm&udblac;vel)</klr></trd>,
          <trd>utazik</trd>
        </trdgrp>
      </snc>
      <snc>
        <dif>
          Transloki&gcirc;i pp <tld/>iloj:
          <ekz>
            <tld/>as vagonaro<fnt>Z</fnt>,
          </ekz><ekz>
            &scirc;ipo<fnt>B</fnt>,
          </ekz><ekz>
            biciklo<fnt>B</fnt>,
          </ekz><ekz>
            fiakro.
          </ekz>
        </dif>
        <trd lng="hu">k&ouml;zlekedik</trd>
      </snc>
  <trd lng="be">&c_je;&c_h;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">jet</trd>
  <trd lng="de">fahren</trd>
  <trd lng="en">ride</trd>
  <trdgrp lng="es">
        <trd>ir<klr> (en un veh&iacute;culo)</klr></trd>,
        <trd>desplazarse<klr> (en un veh&iacute;culo)</klr></trd>
      </trdgrp>
  <trdgrp lng="fr">
        <trd>aller<klr> (en v&eacute;hicule)</klr></trd>,
        <trd>se d&eacute;placer<klr> (en v&eacute;hicule)</klr></trd>
      </trdgrp>
  <trdgrp lng="nl">
        <trd>reizen</trd>,
        <trd>rijden <klr tip="ind">(in een voertuig)</klr></trd>
      </trdgrp>
  <trdgrp lng="pl">
        <trd>je&zacute;dzi&cacute;</trd>,
        <trd>jecha&cacute;</trd>
      </trdgrp>
  <trdgrp lng="pt">
      <trd>andar de ve&iacute;culo</trd>,
      <trd>ir<klr tip="ind">(com alguma esp&eacute;cie de
          ve&iacute;culo)</klr></trd>
      </trdgrp>
  <trdgrp lng="ru">
        <trd><ofc>*</ofc>&c_je;&c_h;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_je;&c_z;&c_d;&c_i;&c_t;&c_mol;</trd>
      </trdgrp>
  <trdgrp lng="sk">
    <trd>cestovať</trd>,
    <trd>chodiť</trd>,
    <trd>viezť sa</trd>
  </trdgrp>
  <trdgrp lng="sv">
        <trd>&aring;ka</trd>,
        <trd>fara</trd>,
        <trd>f&auml;rdas</trd>
      </trdgrp>
  <trd lng="tr">arabayla bir yerden bir yere gitmek</trd>
</drv>

    <drv mrk="vetur.0igi">
      <kap><tld/>igi</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Transporti per diversaj rimedoj:
          <ekz>
            jen la lokomotivo, kiu nin <tld/>igos;
          </ekz><ekz>
            <tld/>igi grenon al bazaro<fnt>B</fnt>;
          </ekz><ekz>
            li metis la kadavron sur la azenon kaj <tld/>igis &gcirc;in
            returne<fnt>Z</fnt>.
          </ekz>
        </dif>
        <trd lng="be">&c_v;&c_je;&c_z;&c_mol;&c_c;&c_ib;</trd>
        <trdgrp lng="de">
          <trd>transportieren</trd>
        </trdgrp>
        <trdgrp lng="es">
          <trd>transportar</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>transporter</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>sz&aacute;ll&iacute;t</trd>,
          <trd>fuvaroz</trd>
        </trdgrp>
        <trd lng="nl">vervoeren</trd>
        <trd lng="pl">wozi&cacute;</trd>
        <trdgrp lng="ru">
          <trd>&c_v;&c_je;&c_z;&c_t;&c_i;</trd>,
          <trd>&c_v;&c_o;&c_z;&c_i;&c_t;&c_mol;</trd>
        </trdgrp>
        <trdgrp lng="sv">
          <trd>skjutsa</trd>,
          <trd>k&ouml;ra</trd>
        </trdgrp>
      </snc>
      <snc>
        <dif>
          Direkti <tld/>ilon:
          <ekz>
            malgranda kale&scirc;o, <tld/>igata de unu
            knabo<fnt>Z</fnt>.
          </ekz>
        </dif>
        <trdgrp lng="be">
          <trd>&c_k;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
          <trd>&c_v;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>
        </trdgrp>
        <trdgrp lng="es">
          <trd>conducir</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>conduire</trd>,
          <trd>mener<klr> (un v&eacute;hicule)</klr></trd>
        </trdgrp>
        <trd lng="hu">vezet
          <klr tip="ind">(j&aacute;rm&udblac;vet)</klr></trd>
        <trd lng="nl">besturen</trd>
        <trdgrp lng="pl">
          <trd>prowadzi&cacute;</trd>,
          <trd>kierowa&cacute;</trd>
        </trdgrp>
        <trd lng="ru">&c_v;&c_je;&c_s;&c_t;&c_i;<klr>
            (&c_m;&c_a;&c_sh;&c_i;&c_n;&c_u;, &c_k;&c_a;&c_r;&c_je;&c_t;&c_u; &c_i;
            &c_t;.&c_p;.)</klr></trd>
        <trd lng="sv">k&ouml;ra</trd>
        <trd lng="tr">ara&ccedil; s&uuml;rmek</trd>
      </snc>
  <trd lng="pt">transportar</trd>
  <trdgrp lng="sk">
    <trd>riadiť</trd>,
    <trd>viezť</trd>,
    <trd>voziť</trd>
  </trdgrp>
</drv>

    <drv mrk="vetur.0igisto">
      <kap><tld/>igisto</kap>
      <snc mrk="vetur.0igisto.TRA">
        <dif>
          Homo, kiu direktas <tld/>ilon:
          <ekz>
            la <tld/>igisto neniam ion senbezonan eldiris
            <fnt><bib>Ne&gcirc;aBlovado</bib></fnt>.
          </ekz>
        </dif>
        <refgrp tip="sub">
          <ref cel="auxt.0isto">a&ubreve;tisto</ref>,
          <ref cel="kocxer.0o">ko&ccirc;ero</ref>,
          <ref cel="pilot.0o">piloto</ref>,
          <ref cel="sxofor.0o">&scirc;oforo</ref>
        </refgrp>
      </snc>
  <trdgrp lng="be">
        <trd>&c_k;&c_ib;&c_r;&c_o;&c_w;&c_c;&c_a;</trd>,
        <trd>&c_v;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_je;&c_l;&c_mol;</trd>,
        <trd>&c_v;&c_o;&c_z;&c_ch;&c_y;&c_k;</trd>,
        <trd>&c_f;&c_u;&c_r;&c_m;&c_a;&c_n;</trd>
      </trdgrp>
  <trdgrp lng="de">
        <trd>Fahrer</trd>,
      </trdgrp>
  <trdgrp lng="es">
        <trd>conductor</trd>,
        <trd>maquinista</trd>,
        <trd>piloto</trd>
      </trdgrp>
  <trdgrp lng="fr">
        <trd>chauffeur</trd>,
        <trd>conducteur</trd>,
        <trd>pilote</trd>
      </trdgrp>
  <trdgrp lng="hu">
        <trd>(j&aacute;rm&udblac;)<ind>vezet&odblac;</ind></trd>,
        <trd>kocsis</trd>
      </trdgrp>
  <trdgrp lng="pl">
        <trd>kierowca</trd>,
        <trd>przewo&zacute;nik</trd>,
        <trd>wo&zacute;nica<klr>
            (tylko dot. powoz&oacute;w konnych)</klr></trd>,
        <trd>powo&zdot;&aogonek;cy<klr>
            (tylko dot. powoz&oacute;w konnych)</klr></trd>,
        <trd>furman<klr> (tylko dot. powoz&oacute;w
            konnych)</klr></trd>,
        <trd>fornal<klr> (tylko dot. powoz&oacute;w
            konnych)</klr></trd>,
        <trd>szofer<klr> (tylko dot. samochod&oacute;w i
            autobus&oacute;w)</klr></trd>,
        <trd>motorniczy<klr>
            (tylko dot. tramwaj&oacute;w i trolejbus&oacute;w)</klr></trd>
      </trdgrp>
  <trdgrp lng="ru">
        <trd>&c_v;&c_o;&c_z;&c_n;&c_i;&c_c;&c_a;</trd>,
        <trd>&c_k;&c_u;&c_ch;&c_je;&c_r;</trd>,
        <trd>&c_ja;&c_m;&c_shch;&c_i;&c_k;</trd>,
        <trd>&c_i;&c_z;&c_v;&c_o;&c_z;&c_ch;&c_i;&c_k;</trd>,
        <trd>&c_v;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
      </trdgrp>
  <trd lng="sk">povozník</trd>
  <trdgrp lng="sv">
        <trd>f&ouml;rare</trd>,
        <trd>chauff&ouml;r</trd>
      </trdgrp>
  <trdgrp lng="tr">
        <trd>s&uuml;r&uuml;c&uuml;</trd>,
        <trd>&scedil;of&ouml;r</trd>
      </trdgrp>
</drv>

    <drv mrk="vetur.0ilo">
      <kap><ofc>*</ofc><tld/>ilo</kap>
      <snc mrk="vetur.0ilo.TRA">
        <tezrad fak="TRA"/>
        <uzo tip="fak">TRA</uzo>
        <dif>
          Ilo por transportado, konsistanta esence el iaforma kesto,
          kaj plejofte radoj a&ubreve; glitiloj:
          <ekz>
            en somero ni <tld/>as per diversaj <tld/>iloj
            <fnt><bib>F</bib>, <lok>&FE; 34</lok></fnt>;
          </ekz><ekz>
            na&gcirc;<tld/>ilo<fnt>Z</fnt>.
          </ekz>
        </dif>
      </snc>
  <trdgrp lng="be">
        <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_a;&c_r;&c_t;</trd>,
        <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_a;&c_r;&c_t;&c_n;&c_y;
          &c_s;&c_r;&c_o;&c_d;&c_a;&c_k;</trd>,
        <trd>&c_k;&c_a;&c_l;&c_jo;&c_s;&c_y;</trd>,
        <trd>&c_p;&c_a;&c_v;&c_o;&c_z;&c_k;&c_a;</trd>
      </trdgrp>
  <trdgrp lng="de">
        <trd>Fahrzeug</trd>,
      </trdgrp>
  <trdgrp lng="es">
        <trd>medio de <ind>transporte</ind></trd>,
        <trd>veh&iacute;culo</trd>
      </trdgrp>
  <trdgrp lng="fr">
        <trd>moyen de <ind>transport</ind></trd>,
        <trd>v&eacute;hicule</trd>
      </trdgrp>
  <trd lng="hu">j&aacute;rm&udblac;</trd>
  <trd lng="nl">voertuig</trd>
  <trdgrp lng="pl">
        <trd>pojazd</trd>,
        <trd>w&oacute;z</trd>,
        <trd>pow&oacute;z</trd>,
        <trd>fura<klr> (tak&zdot;e pot.)</klr></trd>
      </trdgrp>
  <trdgrp lng="ru">
        <trd>&c_s;&c_r;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o;
          &c_p;&c_je;&c_r;&c_je;&c_d;&c_v;&c_i;&c_zh;&c_je;&c_n;&c_i;&c_ja;</trd>,
        <trd>&c_e;&c_k;&c_i;&c_p;&c_a;&c_zh;</trd>,
        <trd>&c_p;&c_o;&c_v;&c_o;&c_z;&c_k;&c_a;</trd>,
        <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_o;&c_r;&c_t;</trd>
      </trdgrp>
  <trdgrp lng="sk">
    <trd>dopravný prostriedok</trd>,
    <trd>povoz</trd>,
    <trd>vozidlo</trd>
  </trdgrp>
  <trd lng="sv">fordon</trd>
  <trd lng="tr">ta&scedil;&inodot;ma arac&inodot;</trd>
  <trdgrp lng="zh">
        <trd>&#36710;</trd>,
        <trd><klr>tr. </klr>&#36554;<klr> [ch&#275;]</klr></trd>
      </trdgrp>
</drv>

    <drv mrk="vetur.cxirkaux0i">
      <kap>&ccirc;irka&ubreve;<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="vetur.cxirkaux0i.kun_objekto">
        <dif>
          <tld lit="V"/>i &ccirc;irka&ubreve;:
          <ekz>
            la glitveturilo dufoje &ccirc;irka&ubreve;<tld/>is la placon
            <fnt><bib>Fab2</bib>, <lok>Ne&gcirc;a re&gcirc;ino</lok>
            </fnt>.
          </ekz>
        </dif>
        <trdgrp lng="hu">
          <trd>k&ouml;rbeutazik</trd>,
          <trd>megker&uuml;l
            <klr tip="ind">(j&aacute;rm&udblac;vel)</klr></trd>
        </trdgrp>
        <trd lng="pl">obje&zdot;d&zdot;a&cacute;</trd>
        <trdgrp lng="ru">
          <trd>&c_o;&c_b;&c_malmol;&c_je;&c_h;&c_a;&c_t;&c_mol;</trd>,
          <trd>&c_o;&c_b;&c_malmol;&c_je;&c_z;&c_zh;&c_a;&c_t;&c_mol;
          </trd>
        </trdgrp>
      </snc>
      <snc mrk="vetur.cxirkaux0i.absolute">
        <uzo tip="klr">(abs.)</uzo>
        <dif>
          <tld lit="V"/>i en la &ccirc;irka&ubreve;a&jcirc;o:
          <ekz>
            <klr>[la veturilon]</klr> tiris du flavaj bovoj, kun kiuj
            Georgo havis la permeson iom &ccirc;irka&ubreve;<tld/>i
            <fnt><bib>Fab4</bib>, <lok>Historio el la dunoj</lok></fnt>.
          </ekz>
        </dif>
        <trd lng="hu">ker&uuml;l egyet
          <klr tip="ind">(j&aacute;rm&udblac;vel)</klr></trd>
        <trdgrp lng="pl">
          <trd>przeje&zdot;d&zdot;a&cacute; si&eogonek;</trd>,
          <trd>karn&aogonek;&cacute; si&eogonek;<klr> (pot.)</klr></trd>
        </trdgrp>
        <trd lng="ru">
          &c_r;&c_a;&c_z;&c_malmol;&c_je;&c_z;&c_zh;&c_a;&c_t;&c_mol;
          &c_k;&c_r;&c_u;&c_g;&c_o;&c_m;</trd>
      </snc>
    </drv>

    <drv mrk="vetur.el0i">
      <kap>el<tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc mrk="vetur.el0i.KOMUNE">
        <dif>
          Eliri <tld/>ante:
          <ekz>
            li &jcirc;us el<tld/>is hiera&ubreve;.
          </ekz>
        </dif>
        <trdgrp lng="be">
          <trd>&c_z;&c_mol;&c_je;&c_h;&c_a;&c_c;&c_mol;</trd>,
          <trd>&c_v;&c_y;&c_je;&c_h;&c_a;&c_c;&c_mol;</trd>
        </trdgrp>
        <trdgrp lng="de">
          <trd>ausfahren</trd>,
          <trd>herausfahren</trd>,
        </trdgrp>
        <trdgrp lng="es">
          <trd>salir<klr> (en veh&iacute;culo)</klr></trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>sortir<klr> (en v&eacute;hicule)</klr></trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>kiutazik</trd>,
          <trd>kihajt <klr tip="ind">(j&aacute;rm&udblac;vel)</klr>
          </trd>,
          <trd>kij&aacute;r <klr tip="ind">(j&aacute;rm&udblac;vel)
            </klr></trd>
        </trdgrp>
        <trd lng="pl">wyje&zdot;d&zdot;a&cacute;</trd>
        <trdgrp lng="ru">
          <trd>&c_u;&c_je;&c_h;&c_a;&c_t;&c_mol;</trd>,
          <trd>&c_v;&c_y;&c_je;&c_h;&c_a;&c_t;&c_mol;</trd>
        </trdgrp>
      </snc>
      <trd lng="sk">vycestovať</trd>
</drv>

    <drv mrk="vetur.en0i">
      <kap>en<tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc mrk="vetur.en0i.KOMUNE">
        <dif>
          Eniri <tld/>ante:
          <ekz>
            la kale&scirc;o en<tld/>is en la korton<fnt>B</fnt>.
          </ekz>
        </dif>
      </snc>
  <trdgrp lng="be">
        <trd>&c_u;&c_je;&c_h;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_z;&c_a;&c_je;&c_h;&c_a;&c_c;&c_mol;</trd>
      </trdgrp>
  <trdgrp lng="de">
        <trd>einfahren</trd>,
        <trd>hineinfahren</trd>,
      </trdgrp>
  <trdgrp lng="es">
        <trd>entrar<klr> (en veh&iacute;culo)</klr></trd>
      </trdgrp>
  <trdgrp lng="fr">
        <trd>entrer<klr> (avec un v&eacute;hicule)</klr></trd>
      </trdgrp>
  <trdgrp lng="hu">
        <trd>beutazik</trd>,
        <trd>behajt <klr tip="ind">(j&aacute;rm&udblac;vel)</klr></trd>,
        <trd>bej&aacute;r <klr tip="ind">(j&aacute;rm&udblac;vel)</klr>
        </trd>
      </trdgrp>
  <trd lng="pl">wje&zdot;d&zdot;a&cacute;</trd>
  <trdgrp lng="ru">
        <trd>&c_v;&c_malmol;&c_je;&c_h;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_z;&c_a;&c_je;&c_h;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
  <trdgrp lng="sk">
    <trd>vchádzať</trd>,
    <trd>vojsť</trd>
  </trdgrp>
</drv>

    <drv mrk="vetur.0ejo">
      <kap><tld/>ejo</kap>
      <snc mrk="vetur.0ejo.sxoseo">
        <dif>
          Tiu parto de placo, de urba strato a&ubreve; alia vojo, kiu estas
          destinita por <tld/>ado (kontraste al la
          <ref cel="trotua.0o">trotuaroj</ref>, gazonoj ktp):
          <ekz>
            same kiel aliloke, sur stratoj inter <tld/>ejo kaj
            piedirejo estas duonmetra akvofluejo, rande de kiu estas
            la betonbloka <ctl>barilo</ctl>
            <fnt>
              <aut>Matti Lahtinen</aut>:
              <vrk>Kvar semajnoj en Armenio kaj Irano</vrk>
              <lok><url
              ref="http://users.jyu.fi/~mlahtine/ArIr2002/TTT-Unic/tehr.htm"
                  >Tehrano</url></lok>
            </fnt>;
          </ekz><ekz>
            fumo levi&gcirc;as el iu vendejo, kaj la fajrobrigado &jcirc;etas
            malsekajn, fulgajn meblojn sur la <tld/>ejon
            <fnt>
              <aut>Jenn&odblac; Rejt&odblac;, tr. L&aacute;szl&oacute; Bal&aacute;zs</aut>:
              <vrk><url
 ref="http://www.elerno.cn/bbs/dispbbs.asp?boardid=21&amp;id=5233"
                  >Pariza fronto</url></vrk>
            </fnt>;
          </ekz><ekz>
            &scirc;i eksaltis de la <tld/>ejo sur la trotuaron, provis
            rifu&gcirc;i en la enirejo, sed la elfluanta publiko baris al
            &scirc;i la vojon
            <fnt><bib>MkM</bib>, <lok>&ccirc;ap.14a, p.131a</lok></fnt>.
          </ekz>
        </dif>
        <trd lng="en">traffic area</trd>
        <trd lng="es">calzada</trd>
        <trd lng="fr">chauss&eacute;e</trd>
        <trd lng="pl">jezdnia</trd>
        <trd lng="ru">&c_p;&c_r;&c_o;&c_je;&c_z;&c_zh;&c_a;&c_ja; &c_ch;&c_a;&c_s;&c_t;&c_mol;</trd>
      </snc>
      <trd lng="sk">jazdná dráha</trd>
</drv>

    <drv mrk="vetur.glit0ilo">
      <kap><ofc>*</ofc>glit<tld/>ilo</kap>
      <snc mrk="vetur.glit0ilo.TRA">
        <uzo tip="fak">TRA</uzo>
        <dif>
          <ref tip="super" cel="vetur.0ilo.TRA"><tld lit="V"/>ilo</ref>
          kiu staras sur glitiloj por iri sur ne&gcirc;o, glacio,
          eventuale akvo:
          <ekz>
            en vintro ni veturas per glit<tld/>ilo
            <fnt><bib>F</bib>, <lok>&FE; 34</lok></fnt>;
          </ekz><ekz>
            por si li ordonis prepari malgrandan glit<tld/>ilon
            unu&ccirc;evalan
            <fnt><bib>Ne&gcirc;aBlovado</bib></fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="de">Schneemobil</trd>
  <trd lng="es">trineo<klr tip="amb"> (veh&iacute;culo sobre patines)
        </klr></trd>
  <trd lng="fr">traineau<klr tip="amb"> (v&eacute;hicule sur patins)
        </klr></trd>
  <trdgrp lng="pl">
        <trd>sanie</trd>,
        <trd>&sacute;lizgacz</trd>,
        <trd>skuter &sacute;nie&zdot;ny</trd>
      </trdgrp>
  <trd lng="ru"><ofc>*</ofc>&c_s;&c_a;&c_n;&c_i;</trd>
  <trdgrp lng="sk">
    <trd>sane</trd>,
    <trd>sánky</trd>
  </trdgrp>
</drv>

    <drv mrk="vetur.kosmo0o">
      <kap>kosmo<tld/>ilo</kap>
      <snc mrk="vetur.kosmo0ilo.AVI">
        <uzo tip="fak">AVI</uzo>
        <dif>
          <ref tip="super" cel="vetur.0ilo.TRA"><tld lit="V"/>ilo</ref>
          kapabla iri tra la kosmo:
          <ekz>
            la projekto de traktato malpermesas <klr>...</klr> la uzadon
            a&ubreve;
            minacon de forto kontra&ubreve; satelitoj a&ubreve; alia kosmo<tld/>ilo
            <fnt>
              <aut>-</aut>,
              <vrk>Rusio kaj &Ccirc;inio proponas...</vrk>,
              <lok><url ref="http://www.eventeo.net/web/">eventeo</url>,
                2008-02-15</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="es">nave espacial</trd>
      <trd lng="fr">vaisseau spatial</trd>
      <trd lng="pl"><ind>statek</ind> kosmiczny</trd>
    </drv>

    <drv mrk="vetur.pet0i">
      <kap>pet<tld/>i</kap>
      <snc mrk="vetur.pet0i.TRA">
        <uzo tip="fak">TRA</uzo>
        <dif>
          Voja&gcirc;i en veturilo, kies kondukanton oni haltigis sur la
          vojo kaj petis pri transportado:
          <ekz>
            mi ser&ccirc;as amikojn kiuj pet<tld/>adas
            &ccirc;irka&ubreve; la mondo, &scirc;atus
            kunveturi kaj havas sperton pri tio
            <fnt>
              <aut>A. Makarov</aut>,
              <vrk>15 julio 2004, Esperanto Koresponda Servo</vrk>,
              <lok>
                <url
                ref="http://groups.google.be/group/soc.culture.esperanto?hl=eo"
                  >soc.culture.esperanto</url>,
                2004-07-15</lok></fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="be">
        &c_p;&c_a;&c_d;&c_a;&c_r;&c_o;&c_zh;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;
        <ind>&c_a;&c_w;&c_t;&c_a;&c_s;&c_t;&c_o;&c_p;&c_a;&c_m;</ind>
      </trd>
  <trd lng="br">c'hoari <ind>biz-meud</ind></trd>
  <trdgrp lng="de">
        <trd>trampen</trd>,
        <trd>Autostop fahren</trd>,
        <trd>per Anhalter fahren</trd>
      </trdgrp>
  <trd lng="es">viajar haciendo <ind>auto-stop</ind></trd>
  <trdgrp lng="fr">
        <trd>voyager en <ind>auto-stop</ind></trd>,
        <trd>voyager en <ind>stop</ind></trd>
      </trdgrp>
  <trd lng="hu">aut&oacute;stopozik</trd>
  <trdgrp lng="nl">
        <trd>liften</trd>
      </trdgrp>
  <trdgrp lng="pl">
        <trd>podr&oacute;&zdot;owa&cacute; autostopem</trd>,
        <trd>podr&oacute;&zdot;owa&cacute; okazj&aogonek;</trd>
      </trdgrp>
  <trd lng="ru">
        &c_p;&c_u;&c_t;&c_je;&c_sh;&c_je;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;
        &c_mol;&c_a;&c_v;&c_t;&c_o;&c_s;&c_t;&c_o;&c_p;&c_o;&c_m;</trd>
  <trd lng="sk">cestovať autostopom</trd>
  <trd lng="tr">otostop yapmak</trd>
</drv>

    <drv mrk="vetur.post0ilo">
      <kap>post<tld/>ilo</kap>
      <fnt><bib>PIV1</bib></fnt>
      <snc mrk="vetur.post0ilo.TRA">
        <uzo tip="fak">TRA</uzo>
        <dif>
          <tld lit="V"/>ilo trenata de alia <tld/>ilo:
          <ekz>
            ni starigu la tendon kaj malplenigu la a&ubreve;ton
            kaj la post<tld/>ilon
            <fnt>
              <aut>Ch. Bertin</aut>
              <vrk>L'esp&eacute;ranto avec plaisir</vrk>
              <lok>
                <url ref="http://www.ifrance.fr/bertin/catal/eap.htm">
                  http://www.ifrance.fr/bertin/catal/eap.htm</url></lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="es">remolque</trd>
  <trd lng="fr">remorque</trd>
  <trdgrp lng="hu">
        <trd>ut&aacute;nfut&oacute;</trd>,
        <trd>p&oacute;tkocsi</trd>
      </trdgrp>
  <trd lng="pl">przyczepa</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_c;&c_je;&c_p;</trd>
  <trd lng="sk">príves <klr>(vozidlo)</klr></trd>
  <trd lng="sv">sl&auml;p</trd>
  <trd lng="tr">r&ouml;mork</trd>
</drv>

    <drv mrk="vetur.re0i">
      <kap>re<tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc mrk="vetur.re0i.KOMUNE">
        <dif>
          Reveni <tld/>ante.
        </dif>
      </snc>
      <trdgrp lng="es">
        <trd>volver<klr> (en veh&iacute;culo)</klr></trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>revenir<klr> (avec un v&eacute;hicule)</klr></trd>
      </trdgrp>
      <trd lng="be">&c_v;&c_ja;&c_r;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>
      <trd lng="hu">visszautazik</trd>
      <trdgrp lng="pl">
        <trd>wraca&cacute;</trd>,
        <trd>powraca&cacute;</trd>
      </trdgrp>
      <trd lng="ru">&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;
      </trd>
      <trd lng="sv">&aring;ka tillbaka</trd>
    </drv>

    <drv mrk="vetur.tra0i">
      <kap>tra<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="vetur.tra0i.KOMUNE">
        <dif>
          Trairi <tld/>ante:
          <ekz>
            tra<tld/>i la tutan E&ubreve;ropon<fnt>B</fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_je;&c_h;&c_a;&c_c;&c_mol;<klr>
          (&c_s;&c_k;&c_r;&c_o;&c_z;&c_mol; &c_n;&c_je;&c_sh;&c_t;&c_a;,
          &c_c;&c_a;&c_l;&c_k;&c_a;&c_m;)</klr></trd>
  <trdgrp lng="es">
        <trd>cruzar<klr> (en veh&iacute;culo)</klr></trd>,
        <trd>recorrer<klr> (en veh&iacute;culo)</klr></trd>
      </trdgrp>
  <trdgrp lng="fr">
        <trd>passer par<klr> (en v&eacute;hicule)</klr></trd>,
        <trd>parcourir<klr> (en v&eacute;hicule)</klr></trd>
      </trdgrp>
  <trd lng="hu">&aacute;tutazik <klr tip="ind">(vmin)</klr></trd>
  <trd lng="pl">przeje&zdot;d&zdot;a&cacute;</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_je;&c_h;&c_a;&c_t;&c_mol;<klr>
          (&c_c;&c_je;&c_l;&c_i;&c_k;&c_o;&c_m;, &c_ch;&c_je;&c_r;&c_je;&c_z;)
        </klr></trd>
  <trdgrp lng="sk">
    <trd>precestovať</trd>,
    <trd>prejsť cez <klr>(krajinu, bránu)</klr></trd>
  </trdgrp>
  <trd lng="sv">&aring;ka igenom</trd>
</drv>

    <drv mrk="vetur.trans0i">
      <kap>trans<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="vetur.trans0i.KOMUNE">
        <dif>
          Transiri <tld/>ante:
          <ekz>
            trans<tld/>i maron.
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_je;&c_h;&c_a;&c_c;&c_mol;
        <klr>
          (&c_p;&c_r;&c_a;&c_z;&c_mol; &c_n;&c_je;&c_sh;&c_t;&c_a;)
        </klr></trd>
  <trdgrp lng="es">
        <trd>pasar<klr> (en veh&iacute;culo)</klr></trd>,
        <trd>atravesar<klr> (en veh&iacute;culo)</klr></trd>
      </trdgrp>
  <trdgrp lng="fr">
        <trd>passer<klr> (en v&eacute;hicule)</klr></trd>,
        <trd>d&eacute;passer<klr> (en v&eacute;hicule)</klr></trd>
      </trdgrp>
  <trd lng="hu">&aacute;tutazik <klr tip="ind">(vhova)</klr></trd>
  <trd lng="pl">przeje&zdot;d&zdot;a&cacute;</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_je;&c_h;&c_a;&c_t;&c_mol;
        <klr>(&c_ch;&c_je;&c_r;&c_je;&c_z; &c_ch;&c_t;&c_o;-&c_l;.)</klr>
      </trd>
  <trd lng="sk">prejsť <klr>(cez kopec)</klr></trd>
</drv>
