<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vetur</rad>/i
</kap>
<drv mrk="vetur.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="ir.0i">Iri</ref> ne pa&scirc;ante sed transportate
      per diversaj rimedoj:
      <ekz>
        morga&ubreve; mi <tld/>os Parizon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i per kale&scirc;o<fnt>B</fnt>,
      </ekz>
      <ekz>
        per aviadilo, en vagonaro<fnt>B</fnt>,
      </ekz>
      <ekz>
        sur &scirc;ipo<fnt>B</fnt>,
      </ekz>
      <ekz>
        je &ccirc;evalo
        (<ref tip="sin" cel="rajd.0i">rajdi</ref>);
      </ekz>
      <ekz>
        la princo permesis al li <tld/>i kun si kaj ili
        ekgalopis<fnt>B</fnt>;
      </ekz>
      <ekz>
        aer<tld/>i, mar<tld/>i, &scirc;ip<tld/>i, vel<tld/>i
        <klr>(<tld/>i en aero, maro, per &scirc;ipo, velo)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="krur.0o">kruro</ref>,
      <ref cel="pasagx.0o">pasa&gcirc;ero</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Transloki&gcirc;i pp <tld/>iloj:
      <ekz>
        <tld/>as vagonaro<fnt>Z</fnt>,
      </ekz>
      <ekz>
        &scirc;ipo<fnt>B</fnt>,
      </ekz>
      <ekz>
        biciklo<fnt>B</fnt>,
      </ekz>
      <ekz>
        fiakro.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_je;&c_h;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">jet</trd>
  <trd lng="de">fahren</trd>
  <trd lng="en">ride</trd>
  <trdgrp lng="fr">
    <trd>aller<klr> (en v&eacute;hicule)</klr></trd>,
    <trd>se d&eacute;placer<klr> (en v&eacute;hicule)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>reizen</trd>,
    <trd>rijden <klr tip="ind">(in een voertuig)</klr></trd>
  </trdgrp>
  <trd lng="pt">ir<klr tip="ind">(com alguma esp&eacute;cie de ve&iacute;culo)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_je;&c_h;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_je;&c_z;&c_d;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="vetur.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Transporti per diversaj rimedoj:
      <ekz>
        jen la lokomotivo, kiu nin <tld/>igos;
      </ekz>
      <ekz>
        <tld/>igi grenon al bazaro<fnt>B</fnt>;
      </ekz>
      <ekz>
        li metis la kadavron sur la azenon kaj <tld/>igis &gcirc;in
        returne<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_je;&c_z;&c_mol;&c_c;&c_ib;</trd>
    <trdgrp lng="fr">
      <trd>transporter</trd>
    </trdgrp>
    <trd lng="nl">vervoeren</trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_je;&c_z;&c_t;&c_i;</trd>,
      <trd>&c_v;&c_o;&c_z;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Direkti <tld/>ilon:
      <ekz>
        malgranda kale&scirc;o, <tld/>igata de unu knabo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_v;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="nl">besturen</trd>
    <trdgrp lng="fr">
      <trd>conduire</trd>,
      <trd>mener<klr> (un v&eacute;hicule)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_je;&c_s;&c_t;&c_i;<klr> (&c_m;&c_a;&c_sh;&c_i;&c_n;&c_u;, &c_k;&c_a;&c_r;&c_je;&c_t;&c_u; &c_i; &c_t;.&c_p;.)</klr></trd>
  </snc>
  <trd lng="pt">transportar</trd>
</drv>

<drv mrk="vetur.0igisto">
  <kap><tld/>igisto</kap>
  <snc>
    <dif>
      Homo, kiu direktas <tld/>ilon.
    </dif>
    <refgrp tip="sub">
      <ref cel="auxt.0isto">a&ubreve;tisto</ref>,
      <ref cel="kocxer.0o">ko&ccirc;ero</ref>,
      <ref cel="motor.0isto">motoristo</ref>,
      <ref cel="konduk2.0o">konduktoro</ref>,
      <ref cel="sxofor.0o">&scirc;oforo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_r;&c_o;&c_w;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_v;&c_o;&c_z;&c_ch;&c_y;&c_k;</trd>,
    <trd>&c_f;&c_u;&c_r;&c_m;&c_a;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>chauffeur</trd>,
    <trd>conducteur</trd>,
    <trd>pilote</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_z;&c_n;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_k;&c_u;&c_ch;&c_je;&c_r;</trd>,
    <trd>&c_ja;&c_m;&c_shch;&c_i;&c_k;</trd>,
    <trd>&c_i;&c_z;&c_v;&c_o;&c_z;&c_ch;&c_i;&c_k;</trd>,
    <trd>&c_v;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="vetur.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Ilo por transportado, konsistanta esence el iaforma kesto,
      kaj plejofte radoj a&ubreve; glitiloj:
      <ekz>
        en somero ni <tld/>as per diversaj <tld/>iloj, kaj en vintro
        per glit<tld/>ilo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        na&gcirc;<tld/>ilo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_a;&c_r;&c_t;</trd>,
    <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_a;&c_r;&c_t;&c_n;&c_y;
    &c_s;&c_r;&c_o;&c_d;&c_a;&c_k;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_jo;&c_s;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_o;&c_z;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>moyen de <ind>transport</ind></trd>,
    <trd>v&eacute;hicule</trd>
  </trdgrp>
  <trd lng="nl">voertuig</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_r;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o; &c_p;&c_je;&c_r;&c_je;&c_d;&c_v;&c_i;&c_zh;&c_je;&c_n;&c_i;&c_ja;</trd>,
    <trd>&c_e;&c_k;&c_i;&c_p;&c_a;&c_zh;</trd>,
    <trd>&c_p;&c_o;&c_v;&c_o;&c_z;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_o;&c_r;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#36710;</trd>,
    <trd><klr>tr. </klr>&#36554;<klr> [ch&#275;]</klr></trd>
  </trdgrp>
</drv>

<drv mrk="vetur.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Eliri <tld/>ante:
      <ekz>
        li &jcirc;us el<tld/>is hiera&ubreve;.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_mol;&c_je;&c_h;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_je;&c_h;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>sortir<klr> (en v&eacute;hicule)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_u;&c_je;&c_h;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_je;&c_h;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="vetur.en0i">
  <kap>en<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Eniri <tld/>ante:
      <ekz>
        la kale&scirc;o en<tld/>is en la korton<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_je;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_je;&c_h;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>entrer<klr> (avec un v&eacute;hicule)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_malmol;&c_je;&c_h;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_je;&c_h;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="vetur.re0i">
  <kap>re<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Reveni <tld/>ante.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>revenir<klr> (avec un v&eacute;hicule)</klr></trd>
  </trdgrp>
  <trd lng="be">&c_v;&c_ja;&c_r;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>
  <trd lng="ru">&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="vetur.tra0i">
  <kap>tra<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Trairi <tld/>ante:
      <ekz>
        tra<tld/>i la tutan E&ubreve;ropon<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_je;&c_h;&c_a;&c_c;&c_mol;<klr> (&c_s;&c_k;&c_r;&c_o;&c_z;&c_mol; &c_n;&c_je;&c_sh;&c_t;&c_a;, &c_c;&c_a;&c_l;&c_k;&c_a;&c_m;)</klr></trd>
  <trdgrp lng="fr">
    <trd>passer par<klr> (en v&eacute;hicule)</klr></trd>,
    <trd>parcourir<klr> (en v&eacute;hicule)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_je;&c_h;&c_a;&c_t;&c_mol;<klr> (&c_c;&c_je;&c_l;&c_i;&c_k;&c_o;&c_m;, &c_ch;&c_je;&c_r;&c_je;&c_z;)</klr></trd>
</drv>

<drv mrk="vetur.trans0i">
  <kap>trans<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Transiri <tld/>ante:
      <ekz>
        trans<tld/>i maron.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_je;&c_h;&c_a;&c_c;&c_mol;<klr> (&c_p;&c_r;&c_a;&c_z;&c_mol; &c_n;&c_je;&c_sh;&c_t;&c_a;)</klr></trd>
  <trdgrp lng="fr">
    <trd>passer<klr> (en v&eacute;hicule)</klr></trd>,
    <trd>d&eacute;passer<klr> (en v&eacute;hicule)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_je;&c_h;&c_a;&c_t;&c_mol;<klr> (&c_ch;&c_je;&c_r;&c_je;&c_z; &c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>
</drv>
</art>
