  </trd>
<trd lng="sk">prejs&tcaron;<klr tip="amb"> (cez kopec)</klr></trd>
</drv>

<drv mrk="vetur.sur0i">
  <kap>sur<tld/>i<fnt><bib>PIV1</bib></fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vetur.sur0i.ion">
    <dif>
      <tld lit="V"/>i sur ion:
      <ekz>
        neniam iu triumfanto sur<tld/>is tiel fiere Kapitolon
        <fnt>
          <aut>H. Sienkewicz, tr. Lidia Zamenhof</aut>:
          <vrk>Quo vadis?</vrk>. <lok>&Ccirc;ap. 49&#xAA;</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_v;&c_z;&c_malmol;&c_je;&c_h;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="vetur.sur0i.iun">
    <dif>
      <ref tip="super" cel="akcide.0o">Akcidente</ref> faligi kaj
      <tld/>i sur iun:
      <ekz>
        Vidu, kia grava evento: &ccirc;efredaktoro de beletra revuo
        sur<tld/>ita de tramo!
        <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 11&#xAA;</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="en">run over</trd>
    <trdgrp lng="fr">
      <trd>&eacute;craser<klr tip="amb"> (en v&eacute;hicule)</klr></trd>,
      <trd>renverser<klr tip="amb"> (en v&eacute;hicule)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_d;&c_a;&c_v;&c_i;&c_t;&c_mol;<klr tip="amb"> (&c_o; &c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_o;&c_r;&c_t;&c_n;&c_o;&c_m;
      &c_s;&c_r;&c_je;&c_d;&c_s;&c_t;&c_v;&c_je;)</klr></trd>,
      <trd>&c_n;&c_a;&c_je;&c_h;&c_a;&c_t;&c_mol;<klr tip="amb"> &c_n;&c_a; &c_k;&c_o;&c_g;&c_o; (&c_o; &c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_o;&c_r;&c_t;&c_n;&c_o;&c_m;
      &c_s;&c_r;&c_je;&c_d;&c_s;&c_t;&c_v;&c_je;)</klr></trd>,
      <trd>&c_p;&c_je;&c_r;&c_je;&c_je;&c_h;&c_a;&c_t;&c_mol;<klr tip="amb"> &c_k;&c_o;&c_g;&c_o; (&c_o; &c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_o;&c_r;&c_t;&c_n;&c_o;&c_m;
      &c_s;&c_r;&c_je;&c_d;&c_s;&c_t;&c_v;&c_je;)</klr></trd>
    </trdgrp>
  </snc>
</drv>
