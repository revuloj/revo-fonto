<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: plag.xml,v 1.19 2023/09/03 12:45:08 revo Exp $">
<kap>
    <ofc>4</ofc>
    <rad>plag</rad>/o
    <fnt>Z</fnt>
</kap>
<drv mrk="plag.0o">
    <kap><tld/>o</kap>
    <snc>
      <dif>
        Tutlanda longeda&ubreve;ra katastrofo, ka&ubreve;zita de Dio kiel puno por la
        homoj:
        <ekz>
          <ind>la sep <tld/>oj de Egiptujo</ind>.
          <trd lng="ru">&c_s;&c_je;&c_m;&c_mol; &c_k;&c_a;&c_z;&c_n;&c_je;&c_j; &c_je;&c_g;&c_i;&c_p;&c_je;&c_t;&c_s;&c_k;&c_i;&c_h;</trd>
        </ekz>
      </dif>
      <trd lng="pl">plaga</trd>
    </snc>
    <snc>
      <dif>
        Tutlanda granda longeda&ubreve;ra katastrofo:
        <ekz>
          la ma&scirc;ino povas esti liberigilo a&ubreve; <tld/>o por la homaro.
        </ekz>
      </dif>
    <trdgrp lng="fr">
      <trd>fl&eacute;au</trd>,
      <trd>calamit&eacute;</trd>
     </trdgrp>
    </snc>
  <trdgrp lng="be">
      <trd>&c_b;&c_je;&c_d;&c_s;&c_t;&c_v;&c_a;</trd>, 
      <trd>&c_n;&c_a;&c_v;&c_a;&c_l;&c_a;</trd>, 
      <trd>&c_p;&c_l;&c_ja;&c_g;&c_a;</trd>
    </trdgrp>
  <trdgrp lng="cs">
    <trd>kalamita</trd>,
    <trd>ne&#x161;t&#x11B;st&#xED;</trd>,
    <trd>pohroma</trd>,
    <trd>strast</trd>,
    <trd>trest bo&#x17E;&#xED;</trd>
  </trdgrp>
  <trd lng="de">Plage</trd>
  <trd lng="en">plague</trd>
  <trd lng="es">plaga</trd>
  <trd lng="fr">plaie<klr tip="ind"> (fl&eacute;au)</klr></trd>
  <trdgrp lng="hu">
      <trd>sorscsap&aacute;s</trd>,
      <trd>baj</trd>
    </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x707D;&#x3044;  [<ind>&#x308F;&#x3056;&#x308F;&#x3044;</ind>]</trd>,
    <trd>&#x5929;&#x7F70;  [<ind>&#x3066;&#x3093;&#x3070;&#x3064;</ind>]</trd>,
    <trd>&#x5927;&#x707D;&#x5BB3;  [<ind>&#x3060;&#x3044;&#x3055;&#x3044;&#x304C;&#x3044;</ind>]</trd>,
    <trd>&#x707D;&#x5384;  [<ind>&#x3055;&#x3044;&#x3084;&#x304F;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">plaag</trd>
  <trd lng="pt">praga</trd>
  <trd lng="ru">&c_b;&c_je;&c_d;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>pliaga</trd>,
    <trd>pohroma</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x438;&#x445;&#x43E;</trd>,
    <trd>&#x43D;&#x435;&#x449;&#x430;&#x441;&#x442;&#x44F;</trd>,
    <trd>&#x431;&#x456;&#x434;&#x430;</trd>,
    <trd>&#x411;&#x43E;&#x436;&#x430; &#x43A;&#x430;&#x440;&#x430;</trd>,
    <trd>&#x43B;&#x438;&#x445;&#x43E;&#x43B;&#x456;&#x442;&#x442;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="plag.0i">
    <kap><tld/>i</kap>
    <gra><vspec>tr</vspec></gra>
    <snc>
      <dif>
        Puni per <tld/>o:
        <ekz>
          vian popolon ne <tld/>u<fnt>Z</fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_n;&c_a;&c_s;&c_l;&c_a;&c_c;&c_mol;
&c_b;&c_je;&c_d;&c_s;&c_t;&c_v;&c_a;</trd>
      <trd lng="hu">ver<klr tip="ind"> (pl. isten)</klr></trd>
      <trd lng="ru">&c_n;&c_a;&c_s;&c_l;&c_a;&c_t;&c_mol;
&c_b;&c_je;&c_d;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
    </snc>
    <snc>
      <dif>
        Ruinigi per <tld/>o:
        <ekz>
          la <tld/>ita regiono.
        </ekz>
      </dif>
      <trdgrp lng="be">
        <trd>&c_z;&c_r;&c_u;&c_j;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>, 
        <trd>&c_r;&c_a;&c_z;&c_b;&c_u;&c_r;&c_y;&c_c;&c_mol;</trd>
      </trdgrp>
      <trd lng="hu">csap&aacute;ssal s&uacute;jt</trd>
      <trd lng="ru">&c_r;&c_a;&c_z;&c_r;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>
    </snc>
  <trdgrp lng="cs">
    <trd>potrestat pohromou</trd>,
    <trd>zni&#x10D;it pohromou</trd>
  </trdgrp>
  <trd lng="de">plagen</trd>
  <trd lng="en">plague</trd>
  <trd lng="es">sufrir una plaga</trd>
  <trdgrp lng="ja">
    <trd>&#x5929;&#x7F70;&#x3092;&#x4E0B;&#x3059;  [<ind>&#x3066;&#x3093;&#x3070;&#x3064;&#x3092;&#x304F;&#x3060;&#x3059;</ind>]</trd>,
    <trd>&#x8352;&#x5EC3;&#x3055;&#x305B;&#x308B;  [<ind>&#x3053;&#x3046;&#x306F;&#x3044;&#x3055;&#x305B;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">teisteren</trd>
  <trdgrp lng="sk">
    <trd>pliagou</trd>,
    <trd>tresta&#x165; pohromou</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&#x430;&#x441;&#x438;&#x43B;&#x430;&#x442;&#x438; &#x43B;&#x438;&#x445;&#x43E;</trd>,
    <trd>&#x43A;&#x430;&#x440;&#x443;</trd>,
    <trd>&#x43D;&#x435;&#x449;&#x430;&#x441;&#x442;&#x44F;</trd>,
    <trd>&#x431;&#x456;&#x434;&#x443;</trd>,
    <trd>&#x437;&#x430;&#x432;&#x434;&#x430;&#x432;&#x430;&#x442;&#x438; &#x43B;&#x438;&#x445;&#x430;/&#x433;&#x43E;&#x440;&#x44F;</trd>,
    <trd>&#x43A;&#x430;&#x440;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x440;&#x443;&#x439;&#x43D;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x441;&#x43F;&#x443;&#x441;&#x442;&#x43E;&#x448;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>
</art>
