<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: plag.xml,v 1.22 2023/10/20 16:38:17 revo Exp $">
<kap>
    <ofc>4</ofc>
    <rad>plag</rad>/o
    <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="plag.0o">
  <kap><tld/>o</kap>
  <snc mrk="plag.0o.dia">
    <dif>
      Tutlanda longeda&ubreve;ra katastrofo, ka&ubreve;zita de Dio kiel <ref tip="super" cel="pun.0o">puno</ref>
      por la
      homoj:
      <ekz>
        <ind>la sep <tld/>oj de Egiptujo</ind>.
        <trd lng="ru">&c_s;&c_je;&c_m;&c_mol; &c_k;&c_a;&c_z;&c_n;&c_je;&c_j; &c_je;&c_g;&c_i;&c_p;&c_je;&c_t;&c_s;&c_k;&c_i;&c_h;</trd>
      </ekz>
      <ekz>
        Li turnos sur vin <klr>[&#8230;]</klr> &ccirc;ian malsanon kaj &ccirc;ian <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 28:61</lok>
        </fnt>;
      </ekz>
      <ekz>
        per tiuj tri <tld/>oj triono de la homoj estis mortigita
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 9:18</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="pl">plaga</trd>
  </snc>
  <snc mrk="plag.0o.katastrofo">
    <dif>
      Tutlanda granda longeda&ubreve;ra <ref tip="super" cel="katast.0o">katastrofo</ref>:
      <ekz>
        la <tld/>on de fajro estis sekvonta la plago de malsato kaj de malsanoj
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XLVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de civila milito, de murdoj kaj de malsato
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XLVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        sarso <klr>[&#8230;]</klr> povus fari&gcirc;i simila <tld/>o kia <klr>[&#8230;]</klr> aidoso
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2003/008129.html"
              >Propagando dan&gcirc;era</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de migrantaj akridoj neniigis 4 milionojn da hektaroj en Ma&ubreve;ritanio
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2006/008814.php"
              >E&ubreve;ropo plialtigas sian drataron</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="infest.0o">infesto</ref>,
      <ref cel="traged.0o.FIG">tragedio<sncref/></ref>
    </refgrp>
    <trdgrp lng="fr">
      <trd>fl&eacute;au</trd>,
      <trd>calamit&eacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="plag.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Grava longda&ubreve;ra <ref tip="super" cel="gxen.0ajxo">&gcirc;eno</ref>:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kian komercon? &ndash; &scirc;i diris indiferente, <klr>[&#8230;]</klr> &ndash; mi vendis la bienon, kaj
        do tiu <tld/>o <klr>(da&ubreve;ra <ref cel="gxen.0ajxo">&gcirc;eno</ref>)</klr> fini&gcirc;is
        <fnt>
          <bib>HsT</bib>
          <lok>Sia propra mastro</lok>
        </fnt>;
      </ekz>
      <ekz>
        ratoj: pri tiu <tld/>o de la &scirc;ipoj plendas la marveturaj kronikistoj de
        <nom>Pigafetta</nom> &gcirc;is <nom>Melville</nom>
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 25</lok>
        </fnt>;
      </ekz>
      <ekz>
        la hungaraj presejoj ja havis &ccirc;iujn supersignojn, sed la eksterlandaj ne
        <klr>[&#8230;]</klr> la skribma&scirc;ino estis vera <tld/>o, &gcirc;enerale oni aplikis nur la
        oftajn supersignitajn vokalojn
        <fnt>
          <bib>Monato</bib>
          <vrk>Lingvo agonianta</vrk>
        </fnt>;
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_je;&c_d;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_a;&c_l;&c_a;</trd>,
    <trd>&c_p;&c_l;&c_ja;&c_g;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kalamita</trd>,
    <trd>ne&scaron;t&ecaron;st&iacute;</trd>,
    <trd>pohroma</trd>,
    <trd>strast</trd>,
    <trd>trest bo&zcaron;&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Plage</trd>,
    <trd>Heimsuchung</trd>
  </trdgrp>
  <trd lng="en">plague</trd>
  <trd lng="es">plaga</trd>
  <trd lng="fr">plaie<klr tip="ind"> (fl&eacute;au)</klr></trd>
  <trdgrp lng="hu">
    <trd>sorscsap&aacute;s</trd>,
    <trd>baj</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#28797;&#12356;  <pr>&#12431;&#12374;&#12431;&#12356;</pr></trd>,
    <trd>&#22825;&#32624;  <pr>&#12390;&#12435;&#12400;&#12388;</pr></trd>,
    <trd>&#22823;&#28797;&#23475;  <pr>&#12384;&#12356;&#12373;&#12356;&#12364;&#12356;</pr></trd>,
    <trd>&#28797;&#21380;  <pr>&#12373;&#12356;&#12420;&#12367;</pr></trd>
  </trdgrp>
  <trd lng="nl">plaag</trd>
  <trd lng="pt">praga</trd>
  <trd lng="ru">&c_b;&c_je;&c_d;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>pliaga</trd>,
    <trd>pohroma</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_l;&c_i;&c_h;&c_o;</trd>,
    <trd>&c_n;&c_je;&c_shch;&c_a;&c_s;&c_t;&c_ja;</trd>,
    <trd>&c_b;&c_ib;&c_d;&c_a;</trd>,
    <trd>&c_B;&c_o;&c_zh;&c_a; &c_k;&c_a;&c_r;&c_a;</trd>,
    <trd>&c_l;&c_i;&c_h;&c_o;&c_l;&c_ib;&c_t;&c_t;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="plag.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="plag.0i.puni">
    <dif>
      <ref tip="super" cel="pun.0i">Puni</ref> per <tld/>o:
      <ekz>
        ni opiniis lin <tld/>ita, frapita de Dio, kaj humiligita
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 53:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        vian popolon ne <tld/>u
        <fnt>
          <bib>MT</bib>
          <lok>I. Kroniko 21:17</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="turmen.0i">turmenti</ref>
    </refgrp>
    <trd lng="be">&c_n;&c_a;&c_s;&c_l;&c_a;&c_c;&c_mol;
&c_b;&c_je;&c_d;&c_s;&c_t;&c_v;&c_a;</trd>
    <trd lng="hu">ver<klr tip="ind"> (pl. isten)</klr></trd>
    <trd lng="ru">&c_n;&c_a;&c_s;&c_l;&c_a;&c_t;&c_mol;
&c_b;&c_je;&c_d;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="plag.0i.ruinigi">
    <dif>
      <ref tip="super" cel="ruin.0igi">Ruinigi</ref> per <tld/>o:
      <ekz>
        teroro da&ubreve;re plagis kaj mortigis centmilojn da homoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2003/008209.html"
              >Ekscita jaro</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        la okupaciajn armeojn <tld/>as sabotado kaj ka&scirc;pafistoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2003/008191.html"
              >Kompatindaj irakanoj</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        aliaj spertas ko&scirc;maran infana&gcirc;on <klr>[kies sekvoj]</klr> persistas dum la tuta
        vivo kaj <tld/>as &gcirc;is preska&ubreve;suicido
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2006/009002.php"
              >Memoroj neniam perdas sian mal&gcirc;ojon</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="infest.0i">infesti</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_z;&c_r;&c_u;&c_j;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_b;&c_u;&c_r;&c_y;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="hu">csap&aacute;ssal s&uacute;jt</trd>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_r;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>potrestat pohromou</trd>,
    <trd>zni&ccaron;it pohromou</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>plagen</trd>,
    <trd>heimsuchen</trd>,
    <trd>&uuml;berkommen</trd>,
    <trd>verfolgen <klr>(plagen)</klr></trd>
  </trdgrp>
  <trd lng="en">plague</trd>
  <trd lng="es">sufrir una plaga</trd>
  <trdgrp lng="ja">
    <trd>&#22825;&#32624;&#12434;&#19979;&#12377;  <pr>&#12390;&#12435;&#12400;&#12388;&#12434;&#12367;&#12384;&#12377;</pr></trd>,
    <trd>&#33618;&#24259;&#12373;&#12379;&#12427;  <pr>&#12371;&#12358;&#12399;&#12356;&#12373;&#12379;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">teisteren</trd>
  <trdgrp lng="sk">
    <trd>pliagou</trd>,
    <trd>tresta&tcaron; pohromou</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_a;&c_s;&c_i;&c_l;&c_a;&c_t;&c_i; &c_l;&c_i;&c_h;&c_o;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_u;</trd>,
    <trd>&c_n;&c_je;&c_shch;&c_a;&c_s;&c_t;&c_ja;</trd>,
    <trd>&c_b;&c_ib;&c_d;&c_u;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &c_l;&c_i;&c_h;&c_a;/&c_g;&c_o;&c_r;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_u;&c_j;&c_n;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_p;&c_u;&c_s;&c_t;&c_o;&c_sh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
</art>
