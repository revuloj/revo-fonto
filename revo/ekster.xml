      <trd>ext&eacute;rieur<klr tip="ind"> (adj.)</klr></trd>,
      <trd>externe<klr tip="ind"> (adj.)</klr></trd>,
      <trd>superficiel</trd>
    </trdgrp>
  <trd lng="he">&het;&yod;&tsadi;&vav;&nun;&yod;</trd>
  <trdgrp lng="hu">
      <trd>k&uuml;ls&odblac;</trd>,
      <trd>kinti</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5916;&#x306E;  <pr>&#x304C;&#x3044;&#x306E;</pr></trd>,
    <trd>&#x5916;&#x90E8;&#x306E;  <pr>&#x304C;&#x3044;&#x3076;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
