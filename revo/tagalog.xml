<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tagalog.xml,v 1.11 2024/05/10 08:31:28 revo Exp $">
<kap>
  <rad>tagalog</rad>/o <fnt><bib>LanLin</bib></fnt>
</kap>

<drv mrk="tagalog.0o">
  <kap><tld/>o</kap>
  <snc mrk="tagalog.0o.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      Ano de popolo el Filipinoj.
      <ekz>
        la <tld lit="T"/>a, la lingvo de la plej potenca etno (la 
        <tld lit="T"/>oj), estas la tut-Filipina komunikilo
        <fnt>
        <aut>Victor Medrano</aut>,
        <vrk><url ref="http://ca.geocities.com/vixcafe/esperanto/fil.html">Lingvoj
        en Filipinoj</url></vrk>,
        <lok>2005-11-08</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_g;&c_a;&c_l;</trd>
  <trd lng="ca">tag&agrave;log<klr> (individu)</klr></trd>
  <trd lng="de">Tagale</trd>
  <trd lng="fr">tagalog<klr> (peuple)</klr></trd>
  <trd lng="hu">tagalog</trd>
  <trd lng="nl">Tagalog<klr tip="ind"> (volk)</klr></trd>
</drv>

<drv mrk="tagalog.0alingvo">
  <kap><tld/>a lingvo</kap>
  <snc mrk="tagalog.0alingvo.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      Lingvo de la <tld/>oj, bazo de la nacia filipina lingvo.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_g;&c_a;&c_l;&c_mol;&c_s;&c_k;&c_a;&c_ja; &c_m;&c_o;&c_v;&c_a;</trd>,
    <trd>&c_f;&c_ib;&c_l;&c_ib;&c_p;&c_ib;&c_n;&c_s;&c_k;&c_a;&c_ja; &c_m;&c_o;&c_v;&c_a;</trd>,
    <trd>&c_t;&c_a;&c_g;&c_a;&c_l;&c_jo;&c_g;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_t;&c_a;&c_g;&c_a;&c_l;&c_o;&c_g;<klr> (&c_je;&c_z;&c_i;&c_k;)</klr></trd>,
    <trd>&c_t;&c_a;&c_g;&c_a;&c_l;&c_s;&c_k;&c_i; &c_je;&c_z;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">tag&agrave;log<klr> (llengua)</klr></trd>
  <trd lng="de">Tagalog<klr tip="ind"> (Sprache)</klr></trd>
  <trd lng="fr">tagalog<klr> (langue)</klr></trd>
  <trd lng="hu">tagalog<klr tip="ind"> (pl. nyelv)</klr></trd>
  <trd lng="ja">&#x30BF;&#x30AC;&#x30ED;&#x30B0;&#x8A9E;  <pr>&#x30BF;&#x30AC;&#x30ED;&#x30B0;&#x3054;</pr></trd>
  <trd lng="nl">Tagalog<klr tip="ind"> (taal)</klr></trd>
  <trd lng="uk">&c_t;&c_a;&c_g;&c_a;&#x43B;&#x43E;&c_g;</trd>
  <trdgrp lng="zh">
    <trd>&#x5854;&#x52A0;&#x6D1B;&#x8A9E; <pr>t&#x1CE;ji&#x101;lu&#xF2;y&#x1D4;</pr></trd>,
    <trd>&#x5854;&#x52A0;&#x6D1B;&#x8BED; <pr>t&#x1CE;ji&#x101;lu&#xF2;y&#x1D4;</pr></trd>,
    <trd>&#x4ED6;&#x52A0;&#x797F;&#x8A9E; <pr>t&#x101;ji&#x101;l&#xF9;y&#x1D4;</pr></trd>,
    <trd>&#x4ED6;&#x52A0;&#x7984;&#x8BED; <pr>t&#x101;ji&#x101;l&#xF9;y&#x1D4;</pr></trd>
  </trdgrp>
</drv>
</art>

