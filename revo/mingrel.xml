<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mingrel.xml,v 1.4 2025/04/06 03:09:26 afido Exp $">
<kap>
    <rad>mingrel</rad>/o
</kap>
<drv mrk="mingrel.0o">
  <kap><tld/>o</kap>
  <snc mrk="mingrel.0o.ETN">
    <dif>
      Ano de <ref tip="lst" cel="popol.0o.etno" lst="voko:popolanoj">popolo</ref> vivanta precipe en
      la okcidenta parto de <ref tip="malprt" cel="kartvel.K0ujo">Kartvelujo</ref>:
      <ekz>
        en iliajn teritoriojn migris aliaj popoloj, nome rusoj, grekoj, armenoj kaj, plej ofte,
        <tld/>oj
        <fnt>
          <aut>-</aut>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Ab%C4%A5aza_lingvo">
          Ab&hcirc;aza lingvo</url></vrk>
          <lok>Vikipedio, 2024-11-11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ab">&c_a;&c_g;&c_y;&c_r;&c_u;&c_a;</trd>
  <trd lng="be">&c_m;&c_ib;&c_n;&c_g;&c_r;&c_e;&c_l;</trd>
  <trdgrp lng="de">
    <trd>Megrele</trd>,
    <trd>Kolcher</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Mingrelian</trd>,
    <trd>Megrelian</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>m&eacute;gr&eacute;lien<klr> (subst.)</klr></trd>,
    <trd>mingr&eacute;lien<klr> (subst.)</klr></trd>
  </trdgrp>
<trd lng="ka">&#4315;&#4308;&#4306;&#4320;&#4308;&#4314;&#4308;&#4305;&#4312;</trd>
<trd lng="ru">&c_m;&c_je;&c_g;&c_r;&c_je;&c_l;</trd>
</drv>

<drv mrk="mingrel.0a">
  <kap><tld/>a</kap>
  <snc mrk="mingrel.0a.KOMUNE">
    <dif>
      Rilata la <tld/>oj, al ilia kulturo...
    </dif>
  </snc>
  <snc mrk="mingrel.0a.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      La mingrela <ref tip="lst" lst="voko:lingvoj" cel="lingv.0o.homa">lingvo</ref>:
      <ekz>
        la situacio tiam estis dan&gcirc;era precipe por eksterlandanoj, kiuj scipovis bone nek la lokajn
        lingvojn, la kartvalan kaj <tld/>an, nek la oficialan rusan
        <fnt>
          <aut>P. Chrdle</aut>
          <vrk>Sen laco por paco</vrk>
          <lok>2019</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_n;&c_g;&c_r;&c_e;&c_l;&c_mol;&c_s;&c_k;&c_ib;</trd>
  <trd lng="de">megrelisch</trd>
  <trdgrp lng="en">
    <trd>Mingrelian</trd>,
    <trd>Megrelian</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>m&eacute;gr&eacute;lien<klr> (adj.)</klr></trd>,
    <trd>mingr&eacute;lien<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>
</art>

