<art mrk="$Id: logistik.xml,v 1.17 2023/09/21 08:30:25 revo Exp $">
<kap>
  <rad>logistik</rad>/o
</kap>
<drv mrk="logistik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="dif" cel="logik.matematika0o">Matematika logiko</ref>:
      <ekz>
        Couturat estis unu el la fondintoj de <tld/>o
        <fnt><bib>PIV1</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      Ne konfuzu kun:
      <ref cel="logxistik.0o">lo&gcirc;istiko</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_t;&c_e;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_l;&c_jo;&c_g;&c_ib;&c_k;&c_a;</ind></trd>
  <trd lng="cs">logistika</trd>
  <trdgrp lng="de">
    <trd>mathematische <ind>Logik</ind></trd>,
    <trd>Logistik<klr> [ark, evi]</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>mathematical <ind>logic</ind></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd><ind>log&iacute;stica</ind> (matem&aacute;tica)</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_m;&a_n;&a_T;&a_q; &a_r;&f_y;&a_A;&a_D;&f_y;</trd>,
    <trd>&a_m;&a_n;&a_T;&a_q; &a_n;&a_m;&a_A;&a_d;&f_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>logique math&eacute;matique</trd>,
    <trd>logistique<klr tip="ind"> (logique math&eacute;matique)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>form&aacute;lis <ind>logika</ind></trd>,
    <trd>matematikai <ind>logika</ind></trd>,
    <trd>szimbolikus<ind> logika</ind></trd>
  </trdgrp>
  <trd lng="id"><ind>logika</ind> (matematika)</trd>
  <trdgrp lng="nl">
    <trd>logistiek<klr tip="ind"> (symbolische logica)</klr></trd>,
    <trd>symbolische <ind>logica</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>logika matematyczna</trd>,
    <trd>logistyka</trd>
  </trdgrp>
  <trd lng="pt">l&oacute;gica matem&aacute;tica</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_t;&c_je;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja; <ind>&c_l;&c_o;&c_g;&c_i;&c_k;&c_a;</ind></trd>,
    <trd>&c_l;&c_o;&c_g;&c_i;&c_s;&c_t;&c_i;&c_k;&c_a;<klr tip="ind"> (&c_u;&c_s;&c_t;.)</klr></trd>
  </trdgrp>
  <trd lng="sk">logistika</trd>
  <trd lng="uk">&#x43B;&#x43E;&c_g;&c_ib;&c_s;&c_t;&c_i;&#x43A;&c_a; (&c_r;&#x43E;&c_z;&c_d;&c_ib;&#x43B; &#x43C;&c_a;&c_t;&c_je;&#x43C;&c_a;&c_t;&c_i;&#x43A;&c_i;)</trd>
</drv>
</art>
