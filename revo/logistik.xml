<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>logistik</rad>/o
</kap>
<drv mrk="logistik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
     <ref tip="dif" cel="logik.matematika0o">Matematika logiko</ref>:
     <ekz>
       Couturat estis unu el la fondintoj de <tld/>o
       <fnt><bib>PIV1</bib></fnt>.
     </ekz>
    </dif>
    <refgrp tip="vid">
      Ne konfuzu kun:
      <ref cel="logxistik.0o">lo&gcirc;istiko</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>mathematische <ind>Logik</ind></trd>,
    <trd>Logistik</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>mathematical <ind>logic</ind></trd>,
    <trd>logistics</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>logique math&eacute;matique</trd>,
    <trd>logistique<klr tip="ind"> (logique math&eacute;matique)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>form&aacute;lis <ind>logika</ind></trd>,
    <trd>matematikai <ind>logika</ind></trd>,
    <trd>szimbolikus<ind>logika</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_t;&c_je;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja; <ind>&c_l;&c_o;&c_g;&c_i;&c_k;&c_a;</ind></trd>,
    <trd>&c_l;&c_o;&c_g;&c_i;&c_s;&c_t;&c_i;&c_k;&c_a;</trd>
  </trdgrp></drv>
</art>

