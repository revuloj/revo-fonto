<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Anatoli</rad>/o
    </kap>
    <drv mrk="anatoli.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
        <dif>
          Granda duoninsulo inter la maroj Nigra, Egea kaj
          Mediteraneo; la Azia parto de <ref cel="turk.0ujo">Turkujo</ref>.
        </dif>
      </snc>
      <trd lng="en">Anatolia</trd>
      <trd lng="hu">Anat&oacute;lia</trd>
      <trdgrp lng="ru">
        <trd>&c_M;&c_a;&c_l;&c_a;&c_ja; &c_A;&c_z;&c_i;&c_ja;</trd>,
        <trd>&c_A;&c_n;&c_a;&c_t;&c_o;&c_l;&c_i;&c_ja;</trd></trdgrp>
      <trd lng="tr">Anadolu</trd>
    </drv>
  </art>

