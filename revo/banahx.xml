      <ref cel="spac.banahxa0o"><tld lit="b"/>a spaco</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Banachsch</trd>
  <trd lng="en">Banach['s]</trd>
  <trd lng="fr">de <ind>Banach</ind></trd>
  <trd lng="ru">&c_b;&c_a;&c_n;&c_a;&c_h;&c_o;&c_v;</trd>
</drv>
</art>

