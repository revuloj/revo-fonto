<drv mrk="banahx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <dif>
      Pollingve: Stefan <trd lng="pl"><klr tip="amb">Stefan</klr>
