    <trdgrp lng="hu">
      <trd>k&aacute;lyha</trd>,
      <trd>t&udblac;zhely</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>piec <klr>grzewczy</klr></trd>,
      <trd>grzejnik</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_je;&c_ch;&c_mol;<klr tip="amb">
    (&c_s;&c_u;&c_shch;.)</klr></trd>
  </snc>
  <trd lng="cs">pokojov√° kamna</trd>
  <trd lng="sk">kachle</trd>
</drv>
