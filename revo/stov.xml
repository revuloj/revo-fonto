<drv mrk="stov.0o">
  <kap><tld/>o</kap>
  <snc mrk="stov.0o.TEK">
    <ref tip="dif" cel="forn.hejto0o.HEJM">hejtoforno</ref>
    <ekz>
      du oldaj servistinoj varmigis vinon, ka&ubreve;rante ekstere
