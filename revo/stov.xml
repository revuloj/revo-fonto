    <refgrp tip="vid">
      <ref cel="fornel.0o">fornelo</ref>,
      <ref cel="hejt.0ilo">hejtilo</ref>
    </refgrp>
    <trd lng="be">&c_p;&c_je;&c_ch;</trd>
