<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: stov.xml,v 1.10 2019/10/19 13:10:22 revo Exp $">
<kap>
  <rad>stov</rad>/o
  <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="stov.0o">
  <kap><tld/>o</kap>
  <snc mrk="stov.0o.TEK">
    <dif>
      <ref tip="dif" cel="forn.hejto0o.HEJM">Hejtoforno</ref>:
      <ekz>
        du oldaj servistinoj varmigis vinon, ka&ubreve;rante ekstere
        &ccirc;e <tld/>o
        <fnt>
          <bib>RugxDom</bib>,
          <lok>&ccirc;apitro 63a, volumo 2a, p. 359a</lok>
        </fnt>;
      </ekz>
      <ekz>
        en mas-<tld/>o oni uzas molan lignon, kiu brulas rapide
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Mas-stovo">Mas-stovo - Vikipedio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fornel.0o">fornelo</ref>,
      <ref cel="hejt.0ilo">hejtilo</ref>
    </refgrp>
    <trd lng="be">&c_p;&c_je;&c_ch;</trd>
    <trd lng="fr">po&ecirc;le<klr> (appareil de chauffage)</klr></trd>
    <trdgrp lng="hu">
      <trd>k&aacute;lyha</trd>,
      <trd>t&udblac;zhely</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>piec <klr>grzewczy</klr></trd>,
      <trd>grzejnik</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_je;&c_ch;&c_mol;<klr tip="amb">
    (&c_s;&c_u;&c_shch;.)</klr></trd>
  </snc>
  <trd lng="cs">pokojov&aacute; kamna</trd>
  <trd lng="de">Heizofen</trd>
  <trd lng="sk">kachle</trd>
</drv>
</art>
