<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: darmsxtati.xml,v 1.6 2023/11/04 13:16:26 revo Exp $">
<kap>
  <rad>darm&scirc;tati</rad>/o
</kap>
<drv mrk="darmsxtati.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <mlg kod="IUPAC">Ds</mlg>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="110">elemento</ref> kun
      atomnumero 110, atommaso 281 u, izotopoj kun atommasoj inter 267
      kaj 282; radioaktiva, rapide disfalanta transirmetalo:
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> estis nomita la&ubreve; la germana urbo Darm&scirc;tato, kie &gcirc;i estis
        unue farita en la jaro 1994 per kunfandi&gcirc;o de plumbo kaj nikelo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=29462">Darm&scirc;tatio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_r;&c_m;&c_sh;&c_t;&c_a;&c_t;&c_y;&c_j;</trd>
  <trd lng="bg">&c_d;&c_a;&c_r;&c_m;&c_shch;&c_a;&c_d;&c_t;&c_i;&c_j;</trd>
  <trd lng="br">darmstadtiom</trd>
  <trd lng="ca">darmstadti</trd>
  <trd lng="de">Darmstadtium</trd>
  <trd lng="en">darmstadtium</trd>
  <trd lng="es">darmstadio</trd>
  <trd lng="fr">darmstadtium</trd>
  <trd lng="hu">darmstadtium</trd>
  <trd lng="id">darmstadtium</trd>
  <trd lng="it">darmstadtio</trd>
  <trd lng="la">darmstadtium</trd>
  <trd lng="nl">darmstadtium</trd>
  <trd lng="pl">darmsztadt</trd>
  <trd lng="pt">darmst&aacute;cio</trd>
  <trd lng="ru">&c_d;&c_a;&c_r;&c_m;&c_sh;&c_t;&c_a;&c_d;&c_t;&c_i;&c_j;</trd>
  <trd lng="sk">darmstadtium</trd>
  <trd lng="sv">darmstadtium</trd>
  <trd lng="zh">&#x943D; <pr>d&#xE1;</pr></trd>
</drv>
</art>

