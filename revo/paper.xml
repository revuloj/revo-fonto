      <trd><ind>tisu</ind> toilet</trd>
    </trdgrp>
  <trd lng="ja">&#x30C8;&#x30A4;&#x30EC;&#x30C3;&#x30C8;&#x30DA;&#x30FC;&#x30D1;&#x30FC;</trd>
  <trd lng="nl">toiletpapier</trd>
