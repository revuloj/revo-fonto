<drv mrk="paper.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      El <tld/>o, rilata al <tld/>o:
      <ekz>
        <tld/>aj floroj
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a &scirc;ipeto
        <fnt>
          <bib>Fab1</bib>
          <lok>Persista stana soldato</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i prenis <tld/>an skatoleton
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj lanternoj radiis kiel grandaj diafanaj tulipoj
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>;
      </ekz>
      <ekz>
        klara kaj bone legebla skribo, sur <tld/>aj folioj,
        havanta la formaton de ordinara letera <tld/>o
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>El la mondo homarana (hilelista).</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la libristo prenis de la tablo la <tld/>an rulpaketon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a strio kun la adreso
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiu decido restos nur sur<tld/>a
        <klr>(skribita sed ne efektivigita)</klr>;
      </ekz>
      <ekz>
        <tld/>aj kaj surdiskaj enciklopedioj
        <fnt>
        <aut>C. Rouget</aut>,
        <vrk>Internet ne (tiom) malbonas</vrk>,
        <bib>Monato</bib>, <lok>1997:8, p. 15a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a versio estas fortike bindita
        <fnt><aut>PP</aut>,
        <vrk>Valora konsultlibro</vrk>,
        <bib>Monato</bib>, <lok>1995:12, p.
        15a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kabine">kabineta</ref>,
      <ref cel="teori.0a">teoria</ref>,
      <ref cel="vort.0a">vorta</ref>
    </refgrp>
  </snc>
  <trd lng="en">paper</trd>
  <trd lng="be">&c_p;&c_a;&c_p;&c_ja;&c_r;&c_o;&c_v;&c_y;</trd>
  <trd lng="br">-<ind>paper</ind></trd>
  <trdgrp lng="de">
    <trd>papieren</trd>,
    <trd>aus <ind>Papier</ind></trd>
  </trdgrp>
  <trd lng="es">de papel</trd>
  <trd lng="fr">de <ind>papier</ind></trd>
  <trdgrp lng="hu">
    <trd>pap&iacute;r-</trd>,
    <trd>pap&iacute;rb&oacute;l k&eacute;sz&uuml;lt</trd>
  </trdgrp>
  <trd lng="ja">&#x7D19;&#x306E;  [<ind>&#x304B;&#x307F;&#x306E;</ind>]</trd>
  <trdgrp lng="nl">
    <trd>papieren</trd>,
    <trd>van <ind>papier</ind></trd>
  </trdgrp>
  <trd lng="pl">papierowy</trd>
  <trd lng="ru">&c_b;&c_u;&c_m;&c_a;&c_zh;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sv">
    <trd>pappers-</trd>,
    <trd>av <ind>papper</ind></trd>
  </trdgrp>
  <trd lng="uk">&c_p;&c_a;&c_p;&c_je;&c_r;&c_o;&c_v;&c_i;&c_j;</trd>
</drv>

<drv mrk="paper.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc>
    <dif>
      Io <tld/>a, objekto el <tld/>o; <tld/>a rubo:
      <ekz>
        el <tld/>a&jcirc;o kaj inko
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; li ku&scirc;is rulpaketo de <tld/>a&jcirc;o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        konteneroj senditaj el Japanio enhavis ne nur <tld/>a&jcirc;ojn,
        sed anka&ubreve; toksajn kaj aliajn dan&gcirc;erajn rubojn
        <fnt>
       <aut>HORI Jasuo/pg</aut>
          <vrk><url ref="&Monato;/2000/006893.php">Rubo-ruzo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/11, p. 14</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ifonoj, metalpecoj kaj <tld/>a&jcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2012/010694.php">Dumvivaj rabatoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        iuj komunumestraroj insistas, ke oni apartigu &jcirc;urnalojn disde aliaj <tld/>a&jcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>John Gobourne</aut>
          <vrk><url ref="&Monato;/2012/010556.php">Tiam ... kaj nun</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_p;&c_ja;&c_r;&c_o;&c_v;&c_y; &c_v;&c_y;&c_r;&c_a;&c_b;</trd>
<trd lng="de">Altpapier</trd>
</drv>

<drv mrk="paper.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="paper.0isto.TIP_paperfaristo">
    <dif>
      <tld lit="P"/>faristo:
      <ekz>
        &ccirc;i tiuj kvar iam laboris &ccirc;e <tld/>isto
        <fnt><aut>E. de Zilah</aut>, <vrk>La Princo &ccirc;e la
        Hunoj</vrk>,
        <lok>2011</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>isto <nom>Franti&scaron;ek Lorenz</nom>, <klr>[&#x2026;]</klr> kuni&gcirc;is kun nove
        venintaj fakuloj en tereno de produktado de <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=199606">Hostinn&eacute;</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Papierhersteller</trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_p;&c_je;&c_r;&c_n;&c_ib;&c_k;</trd>
  <trd lng="en">papermaker</trd>
  <trd lng="cs">pap&iacute;rn&iacute;k</trd>
  <trd lng="fr">papetier</trd>
  <trd lng="pl">papiernik</trd>
  <trd lng="sk">papiern&iacute;k</trd>
</drv>

<drv mrk="paper.0ujo">
  <kap><ofc>*</ofc><tld/>ujo</kap>
  <snc mrk="paper.0ujo.biletujo">
    <dif>
      Koverto el firma materialo, ekz. leda, plasta, portebla en po&scirc;o por konservi
      biletojn, legitimilojn ks; <ref tip="sin" cel="paper.mon0ujo.KOMUNE">mon<tld/>ujo</ref>:
      <ekz>
        en la po&scirc;o de mia pantalono mi portas monujon, kaj en la
        po&scirc;o de mia surtuto mi portas <tld/>ujon
        <fnt><bib>F</bib>, <lok>&FE; 40</lok></fnt>;
      </ekz>
      <ekz>
        la a&ccirc;etanto <klr>[&#x2026;]</klr> tenis grandan dikan mon<tld/>ujon plenan de mono
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">B&ouml;rse</trd>
    <trd lng="en">wallet</trd>
    <trd lng="br">doug-paperio&ugrave;</trd>
    <trdgrp lng="es">
      <trd>billetera</trd>,
      <trd>cartera</trd>
    </trdgrp>
    <trd lng="nl">portefeuille</trd>
    <trd lng="pl">portfel</trd>
    <trd lng="ru">&c_b;&c_u;&c_m;&c_a;&c_zh;&c_n;&c_i;&c_k;</trd>
  </snc>
  <snc mrk="paper.0ujo.dosiera">
    <dif>
      Pli granda po&scirc;o a&ubreve; kovrilo por konservi dokumentojn,
      disajn paperofoliojn, <ref cel="dosier.0o">dosieron</ref>:
      <ekz>
        pli grandan <tld/>ujon mi portas sub la brako
        <fnt><bib>F</bib>, <lok>&FE; 40.</lok></fnt>;
      </ekz>
      <ekz>
        sinjoro <nom>Snitchey</nom> <klr>[&#x2026;]</klr> metis malgrandan bluan akto<tld/>ujon al la tablo
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        el la <tld/>ujo de miaj fabloj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Vitalij</nom> elprenis el <tld/>ujo skizojn kaj planojn de
        moskvaj stratoj
        <fnt><bib>Metrop</bib></fnt>;
      </ekz>
      <ekz>
        nek en la <tld/>ujoj de la kontadejo, nek &ccirc;e la
        financa direktoro, nek en la kabineto de <nom>Latronov</nom> estis
        e&ccirc; malpleja spuro de la kontrakto
