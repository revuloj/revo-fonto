<drv mrk="paper.sur0igi">
  <kap>sur<tld/>igi</kap>
  <snc mrk="paper.sur0igi.redakti">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="redakt.0i.tekston">Redakti</ref>, dokumenti:
      <ekz>
        la a&ubreve;toro fari&gcirc;is poste aktiva esperantisto, kaj nur
        en la lastaj vivo-jaroj decidis sur<tld/>igi siajn fruajn
        spertojn
        <fnt><aut>S. MacGill</aut>, <vrk>Rutine tra tumulto</vrk>,
        <bib>Monato</bib>, <lok>2002:4, p. 20a</lok>
        </fnt>;
      </ekz>
      <ekz>
        li studis la zairan historion, kiel la belgoj &gcirc;in sur<tld/>igis
        <fnt>
          <aut>Wim de Smet</aut>
            <vrk><url ref="&Monato;/2000/007080.php">La flandra en Kongo</url></vrk>
            <bib>Monato</bib>
            <lok>2000/11, p. 4</lok>
          </fnt>;
      </ekz>
      <ekz>
        mi sendis al vi paketon, enhavantan la sur<tld/>igitan manuskripton de la libro
        <fnt>
          <bib>Monato</bib>
          <aut>Filippo</aut>
          <vrk><url ref="&Monato;/2006/008898.php">Moderna po&scirc;to</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        sur<tld/>igita serio de na&ubreve; porradiaj intervjuoj
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="&Monato;/2014/011152.php">Kio estas imperio kaj kiel &gcirc;in
          kontra&ubreve;stari?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">write down</trd>
    <trdgrp lng="be">
      <trd>&c_p;&c_ib;&c_s;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_n;&c_a;&c_p;&c_ib;&c_s;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_k;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol; &c_n;&c_a;
      &c_p;&c_a;&c_p;&c_je;&c_r;&c_y;</trd>
    </trdgrp>
    <trd lng="br">lakaat war baper</trd>
    <trdgrp lng="de">
      <trd>zu Papier bringen</trd>,
      <trd>schreiben</trd>
    </trdgrp>
    <trd lng="fr">coucher sur le papier</trd>
    <trdgrp lng="hu">
      <trd>pap&iacute;ra vet</trd>,
      <trd>le&iacute;r</trd>
    </trdgrp>
    <trd lng="nl">op <ind>papier</ind> zetten</trd>
    <trdgrp lng="pl">
      <trd>przenosi&cacute; na <ind>papier</ind></trd>,
      <trd>uwiecznia&cacute; na <ind>papierze</ind></trd>,
      <trd>zapisywa&cacute;</trd>,
      <trd>pisa&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_n;&c_a;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_i;&c_z;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_mol; &c_n;&c_a;
      &c_b;&c_u;&c_m;&c_a;&c_g;&c_je;</trd>
    </trdgrp>
    <trd lng="sv"><ind>skriva</ind> ner</trd>
  </snc>
  <trdgrp lng="es">
    <trd>escribir</trd>,
    <trd>imprimir</trd>,
    <trd>plasmar en papel</trd>
  </trdgrp>
</drv>

