  &c_h;&c_a;&c_r;&c_t;&c_i;&c_ja;</trd>
  <trd lng="cs">toaletn&iacute; pap&iacute;r</trd>
  <trd lng="de">Toilettenpapier</trd>
  <trd lng="es">papel higi&eacute;nico</trd>
  <trd lng="fr">papier toilette</trd>
  <trdgrp lng="id">
