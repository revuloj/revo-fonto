</drv>

<drv mrk="paper.pauxs0o">
  <kap>pa&ubreve;s<tld/>o</kap> <fnt><bib>PIV1</bib></fnt>
  <snc mrk="paper.pauxs0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Tre diafana papero, kiun oni fiksas sur &scirc;ablono por
      surdesegni kopion, a&ubreve; sur kiun oni printas bildon por
      plua kopiado:
      <ekz>
        <nom>Robert Delaunay</nom>: Tri futbalistoj, plumo sur pa&ubreve;s<tld/>o, 21,3 &#xD7; 15 cm
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=407986">Robert Delaunay</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Pauspapier</trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_mol;&c_k;&c_a; <klr>(&c_p;&c_a;&c_p;&c_je;&c_r;&c_a;)</klr></trd>
  <trd lng="en">tracing paper</trd>
  <trd lng="es">papel cebolla</trd>
