    <trd>papier d'emballage</trd>
  </trdgrp>
  <trd lng="id"><ind>kertas</ind> kado</trd>
  <trdgrp lng="ja">
    <trd>&#x5305;&#x88C5;&#x7D19;  [<ind>&#x307B;&#x3046;&#x305D;&#x3046;&#x3057;</ind>]</trd>,
    <trd>&#x5305;&#x307F;&#x7D19;  [<ind>&#x3064;&#x3064;&#x307F;&#x304C;&#x307F;</ind>]</trd>
  </trdgrp>
