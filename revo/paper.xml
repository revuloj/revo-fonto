<drv mrk="paper.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="paper.0o.materialo">
    <dif>
      Maldika materialo fabrikata el pasto de vegeta&jcirc;aj fibroj, sekigita kaj premita;
      uzata por skribi, presi, paki:
      <ekz>
        <klr>[li]</klr> envolvis ion en pecon da <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili volvis tabakon en strion da <tld/>o kaj fumis tiamaniere
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne volis skribi per <tld/>o kaj inko; sed mi esperas
        veni al vi kaj paroli viza&gcirc;on kontra&ubreve; viza&gcirc;o
        <fnt>
          <bib>NT</bib>
          <lok>II. Johano 1:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        malgrandaj retoj, eltran&ccirc;itajn el diverskolora <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Abio</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili faris &scirc;ipeton el gazeta <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Persista stana soldato</lok>
        </fnt>;
      </ekz>
      <ekz>
        stelo el ora <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Abio</lok>
        </fnt>;
      </ekz>
      <ekz>
        pergamena <tld/>o
        <fnt>
          <aut>Fidel Figueroa</aut>
          <vrk><url ref="&Monato;/2000/007033.php">Nepre &gcirc;ia nomo estas Meksiko</url></vrk>
          <bib>Monato</bib>
          <lok>2000/10, p. 14</lok>
          </fnt>;
      </ekz>
      <ekz>
        <ind>al la <tld/>o ne mankas tolero</ind>
        <klr>(oni povas la&ubreve;vole skribi stulta&jcirc;ojn)</klr>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="hu">a <ind>pap&iacute;r</ind> mindent
        elb&iacute;r</trd>
        <trd lng="nl">papier is geduldig</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="papiru.0o">papiruso</ref>,
      <ref cel="pergam.0o">pergameno</ref>
    </refgrp>
    <trd lng="br">paper</trd>
    <trd lng="pl">papier</trd>
    <trd lng="vo">p&ouml;p</trd>
    <trd lng="tok">lipu</trd>
    <trd lng="zh">&#x7EB8;</trd>
  </snc>

  <snc mrk="paper.0o.folio">
    <dif>
      Folio de tia materialo:
      <ekz>
        blanka <tld/>o ku&scirc;as sur la tablo
        <fnt><bib>F</bib>, <lok>&FE; 10</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;ambro tapetita per flavaj <tld/>etoj
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        konfidi ion al la <tld/>o
        <klr>(= skribi)</klr>;
      </ekz>
      <ekz>
        li <klr>[&#x2026;]</klr> tenis inter la fingroj malgrandan rozokoloran
        <tld/>on, sur kiu trovi&gcirc;is versa&jcirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>

  <snc mrk="paper.0o.dokumento">
    <dif>
      Skribitaj a&ubreve; presitaj notoj, dokumentoj, legitima&jcirc;oj
      kaj similaj:
      <ekz>
        verkisto verkas librojn, kaj skribisto simple transskribas
        <tld/>ojn
        <fnt><bib>F</bib>, <lok>&FE; 37</lok></fnt>;
      </ekz>
      <ekz>
        ordigi siajn <tld/>ojn;
      </ekz>
      <ekz>
        <klr>[li]</klr> en&scirc;ovis kelkajn <tld/>ojn en la po&scirc;on,
        prenis aliajn sub la brakon, por hejme ilin tralegi kaj transskribi
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;an&gcirc;i
        <ind>valor<tld/>o</ind>
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
        <trd lng="de">Wertpapier</trd>
        <trd lng="hu">&eacute;rt&eacute;kpap&iacute;r</trd>
        <trd lng="nl">waardepapier</trd>
      </ekz>
      <ekz>
        rulo da mon<tld/>etoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiam mi (telefone) estis informita ke mi fari&gcirc;is kolonelo,
        certe mi devis iri al la estro por preni la necesajn
        <tld/>ojn
        <fnt>
        <aut>G. L. Arosev</aut>,
        <vrk>Nerekonebla</vrk>,
        <bib>Monato</bib>,
        <lok>2001:5, p. 26a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="br">paperenn</trd>
    <trdgrp lng="pl">
      <trd>papier<klr> (cz&eogonek;sto tylko l.mn.)</klr></trd>
      <trd>akta</trd>
    </trdgrp>
  </snc>
