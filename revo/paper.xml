<drv mrk="paper.karbo0o">
  <kap>karbo<tld/>o</kap> <fnt><bib>PIV1</bib></fnt>
  <snc mrk="paper.karbo0o.KOMUNE">
    <dif>
      <tld lit="P"/>o kun dorsa tavolo de karbopolvo, kiu transkopias la
      skriba&jcirc;on al sub&scirc;ovita folio:
      <ekz>
        la Dua Oficiro tenis notblokon kun en&scirc;ovita karbo<tld/>o kaj krajonon
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 33</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu verkisto <klr>[&#x2026;]</klr> mem devas pagi sian <tld/>on, siajn
        skribilojn, skribma&scirc;inon, inkrubandojn, karbo<tld/>on
        <fnt>
        <bib>Plumamikoj</bib>,
        <lok>la esperanta verkisto: izolo, malprofito, fido</lok>
        </fnt>;
      </ekz>
      <ekz>
        post disvasti&gcirc;o de la skribma&scirc;ino <klr>[&#x2026;]</klr> oni
        povis fari facile dum la skribado de la dokumento traskriba&jcirc;ojn helpe de karbo<tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=413268">Kopi-skribilo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_p;&c_ib;&c_ja;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_p;&c_a;&c_p;&c_je;&c_r;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Pauspapier</trd>,
      <trd>Kohlepapier</trd>
    </trdgrp>
    <trd lng="en">carbon paper</trd>
    <trd lng="fr">papier carbone</trd>
    <trd lng="id"><ind>kertas</ind> karbon</trd>
    <trdgrp lng="nl">
      <trd>carbonpapier</trd>,
      <trd>carbon</trd>,
      <trd>overdrukpapier</trd>,
      <trd>doorslagpapier</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>kalka<klr> maszynowa</klr></trd>,
      <trd>kalka<klr> biurowa</klr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="es">
    <trd>papel carb&oacute;n</trd>,
    <trd>papel calco</trd>
  </trdgrp>
  <trd lng="ja">&#x30AB;&#x30FC;&#x30DC;&#x30F3;&#x7D19;  [<ind>&#x30AB;&#x30FC;&#x30DC;&#x30F3;&#x3057;</ind>]</trd>
  <trd lng="uk">&c_k;&c_o;&c_p;&c_ib;&c_ju;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j; &c_p;&c_a;&c_p;&c_ib;&c_r;</trd>
</drv>

