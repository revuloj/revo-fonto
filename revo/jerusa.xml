<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: jerusa.xml,v 1.25 2023/12/03 07:09:18 revo Exp $">
<kap>
  <rad>Jerusalem</rad>/o <fnt><bib>PIV1</bib></fnt>,
  <var><kap><rad var="z">Jeruzalem</rad>/o  <fnt><bib>FK</bib></fnt></kap></var>
</kap>

<drv mrk="jerusa.J0o">
  <kap><tld/>o, <var><kap><tld var="z"/>o</kap></var></kap>
  <snc mrk="jerusa.J0o.GEOG">
    <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
    <dif>
      La <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&ccirc;efurbo</ref> kaj plej granda
      urbo de <ref tip="malprt" cel="israel.I0o">Israelo</ref>; antikve, anka&ubreve;
      de Izraelio kaj <ref tip="malprt" cel="jud.J0ujo">Judujo</ref>,
      <frm>(35&#x2DA;14'E, 31&#x2DA;46'N)</frm>:
      <ekz>
        li estis apud la sojlo de <tld var="z"/>o, kies turoj blue brilis post la
        intermontoj
        <fnt>
          <bib>FK</bib>
          <vrk>En Kvieta Vespero</vrk>
        </fnt>;
      </ekz>
      <ekz>
        oni venigis lin en <tld/>on, kaj li mortis tie
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 1:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>David</nom> kaj la tuta popolo revenis <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 12:31</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili denove elrampos supren kaj povos rekonstrui <tld var="z"/>on
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        antikvaj Hebreoj tri fojojn &ccirc;iujare kunvenadis en <tld/>o
        <fnt>
          <bib>VivZam</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_Je;&c_r;&c_u;&c_z;&c_a;&c_l;&c_ib;&c_m;</trd>
  <trd lng="bg">&c_J;&c_je;&c_r;&c_u;&c_s;&c_a;&c_l;&c_i;&c_m;</trd>
  <trd lng="ca">Jerusalem</trd>
  <trd lng="cs">Jeruzal&eacute;m</trd>
  <trd lng="de">Jerusalem</trd>
  <trd lng="en">Jerusalem</trd>
  <trd lng="es">Jerusal&eacute;n</trd>
  <trd lng="fr">J&eacute;rusalem</trd>
  <trd lng="he">&yod;&resh;&vav;&shin;&lamed;&yod;&fmem;</trd>
  <trd lng="hr">Jeruzalem</trd>
  <trd lng="hu">Jeruzs&aacute;lem</trd>
  <trd lng="id">Yerusalem</trd>
  <trd lng="it">Gerusalemme</trd>
  <trd lng="ja">&#x30A8;&#x30EB;&#x30B5;&#x30EC;&#x30E0;</trd>
  <trd lng="nl">Jerusalem</trd>
  <trd lng="pt">Jerusal&eacute;m</trd>
  <trd lng="ru">&c_I;&c_je;&c_r;&c_u;&c_s;&c_a;&c_l;&c_i;&c_m;</trd>
  <trd lng="sk">Jeruzalem</trd>
  <trd lng="sv">Jerusalem</trd>
  <trd lng="uk">&#x404;&#x440;&#x443;&#x441;&#x430;&#x43B;&#x438;&#x43C;</trd>
  <trd lng="zh">&#x8036;&#x8DEF;&#x6492;&#x51B7; <pr>Y&#x113;l&#xF9;s&#x101;l&#x11B;ng</pr></trd>
</drv>
</art>

