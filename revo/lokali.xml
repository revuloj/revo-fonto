<art mrk="$Id: lokali.xml,v 1.27 2025/09/15 06:43:03 revo Exp $">
<kap>
  <rad>lokaliz</rad>/i
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="lokali.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="lokali.0i.fiksi">
    <dif>
      <ref tip="super" cel="fiks.0i.flagon">Fiksi</ref> ion en iu loko
      evitante disvasti&gcirc;on:
      <ekz>
        <tld/>i incendion, epidemion, erizipelon;
      </ekz>
      <ekz>
        en la komenco, la kvara nivelo estis atribuita al la epidemio de &leftquot;porka&rightquot; gripo
        la&ubreve; la klasifikado de MOS, &ccirc;ar la epidemio estis <tld/>ita, kaj la intenseco
        de transmisio de gripvirusoj ne estis tro granda
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2009/009882.php"
              >Kelkaj meditoj rilate al la porka gripo</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;efa <tld/>ado de la virusoj de poliomjelito estas la homa intestaro, de kie
        ili eli&gcirc;as kun feka&jcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2011/010269.php"
              >Poliomjelito: &ccirc;u la dua tutmonde likvidota infektmalsano?</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>fixieren</trd>,
      <trd>begrenzen</trd>,
      <trd>eind&auml;mmen</trd>
    </trdgrp>
    <trd lng="en">confine</trd>
    <trdgrp lng="hu">
      <trd>lokaliz&aacute;l</trd>,
      <trd>elszigetel <klr tip="ind"> (egy helyre)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="lokali.0i.loktrovi">
    <dif>
      <ref tip="super" cel="trov.0i.elsercxi">Trovi</ref>, difini la lokon, kie io estas lokita:
      <ekz>
        per radiografado oni povis precize <tld/>i la tumoron;
      </ekz>
      <ekz>
        tiuj doloroj ekaperas en la unuaj semajnoj de la terapio, estas malbone
        <tld/>eblaj (tial oni diras &leftquot;disvasti&gcirc;intaj&#x201D;)
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2013/011124.php"
              >Atentu la koron</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        kontra&ubreve;ulo ne havas malfacila&jcirc;ojn <tld/>i en kiom
        ajn fora mapkvadrato noktan bombaviadilon
        <fnt>
          <lok><url ref="http://miresperanto.narod.ru/biblioteko/aviadistoj/02.htm">
           Vladimir Beekman. Noktaj aviadistoj. El la estona tradukis Jaan
           Ojalo. Redaktis Henrik Seppik. 1989.
           </url>
           </lok>
        </fnt>;
      </ekz>
      <ekz>
        delfeno povas elsendi
        ultrasonan ondon kaj havas la organon por <tld/>i la resonon
        <fnt>
          <lok><url ref="http://www.chinareport.com.cn/08h/08h-01/08h-1-4.htm">
          Zhang Shuang. La blankna&gcirc;ila delfeno Qiqi</url>
          </lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>orten</trd>,
      <trd>aufsp&uuml;ren</trd>,
      <trd>lokalisieren</trd>
    </trdgrp>
    <trd lng="en">locate</trd>
    <trdgrp lng="hu">
      <trd>helymeghat&aacute;roz&aacute;st v&eacute;gez</trd>,
      <trd>meghat&aacute;rozza poz&iacute;ci&oacute;j&aacute;t</trd>,
      <trd>meghat&aacute;rozza helyzet&eacute;t</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>lokalizowa&cacute;</trd>,
      <trd>namierza&cacute;</trd>
    </trdgrp>
    <trd lng="ru">&c_l;&c_o;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>lokumi</ke>, <ke>loktrovi</ke>,
    <ke>lokdetermini</ke>, &#x201A;<ke>trovi lokon de</ke>&#x2018;.
  </rim>
  <trd lng="be">&c_l;&c_ja;&c_k;&c_a;&c_l;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">localitzar</trd>
  <trdgrp lng="cs">
    <trd>lokalizovat</trd>,
    <trd>omezit</trd>
  </trdgrp>
  <trd lng="de">festmachen</trd>
  <trd lng="el">&epsilon;&ny;&tau;&omikron;&pi;&jota_ton;&zeta;&omega;</trd>
  <trd lng="en">localize</trd>
  <trd lng="es">localizar</trd>
  <trdgrp lng="ja">
    <trd>&#x5C40;&#x9650;&#x3059;&#x308B;  <pr>&#x304D;&#x3087;&#x304F;&#x3052;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5E83;&#x304C;&#x308B;&#x306E;&#x3092;&#x98DF;&#x3044;&#x6B62;&#x3081;&#x308B;  <pr>&#x3072;&#x308D;&#x304C;&#x308B;&#x306E;&#x3092;&#x304F;&#x3044;&#x3068;&#x3081;&#x308B;</pr></trd>,
    <trd>&#x5834;&#x6240;&#x3092;&#x3064;&#x304D;&#x3068;&#x3081;&#x308B;  <pr>&#x3070;&#x3057;&#x3087;&#x3092;&#x3064;&#x304D;&#x3068;&#x3081;&#x308B;</pr></trd>,
    <trd>&#x4F4D;&#x7F6E;&#x3092;&#x898B;&#x304D;&#x308F;&#x3081;&#x308B;  <pr>&#x3044;&#x3061;&#x3092;&#x307F;&#x304D;&#x308F;&#x3081;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">lokaliseren</trd>
  <trd lng="pt">localizar</trd>
  <trd lng="ru">&c_l;&c_o;&c_k;&c_a;&c_l;&c_i;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>lokalizova&tcaron;</trd>,
    <trd>obmedzi&tcaron;</trd>,
    <trd>ur&ccaron;i&tcaron;</trd>,
    <trd>vymedzi&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_l;&c_o;&c_k;&c_a;&c_l;&c_ib;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_z;&c_n;&c_a;&c_ch;&c_a;&c_t;&c_i; &c_m;&c_ib;&c_s;&c_c;&c_je; &c_r;&c_o;&c_z;&c_t;&c_a;&c_sh;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_t;&c_a;&c_sh;&c_o;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i; &c_u; &c_v;&c_i;&c_z;&c_n;&c_a;&c_ch;&c_je;&c_n;&c_o;&c_m;&c_u; &c_m;&c_ib;&c_s;&c_c;&c_ib;</trd>
  </trdgrp>
</drv>

<drv mrk="lokali.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">TEK</uzo>
  <uzo tip="fak">BIO</uzo>
  <snc>
    <dif>
      Ilo (aparato a&ubreve; organo) por <tld/>i<sncref ref="lokali.0i.loktrovi"/>
      objekton (ekz-e sonaj a&ubreve; elektromagnetaj ondoj, elsenditaj a&ubreve; reflektitaj de &gcirc;i):
      <ekz>
        la &ccirc;asaviadisto &ccirc;i-momente ne observis ekranon de la
        lokalizilo
        <fnt>
         <lok><url ref="http://miresperanto.narod.ru/biblioteko/aviadistoj/02.htm">
          Vladimir Beekman. Noktaj aviadistoj. El la estona tradukis Jaan
          Ojalo. Redaktis Henrik Seppik. 1989.
          </url>
         </lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bir.0ilo">birilo</ref>,
      <ref cel="naviga.0ilo">navigaciilo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ja;&c_k;&c_a;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_d;&c_e;&c_t;&c_e;&c_k;&c_t;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="ca">localitzador</trd>
  <trdgrp lng="de">
    <trd>Ortungsger&auml;t</trd>,
    <trd>Peilger&auml;t</trd>
  </trdgrp>
  <trd lng="en">locator</trd>
  <trd lng="es">localizador</trd>
  <trd lng="fr">d&eacute;tecteur</trd>
  <trd lng="pl">lokalizator</trd>
  <trd lng="ru">&c_l;&c_o;&c_k;&c_a;&c_t;&c_o;&c_r;</trd>
</drv>

<drv mrk="lokali.radi0ilo">
  <kap>radi<tld/>ilo</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="lokali.0ilo"><tld lit="L"/>ilo</ref>,
      analizanta elektromagnetajn ondojn per radia diapazono.
    </dif>
    <ref tip="vid" cel="radar.0o">radaro</ref>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_d;&c_y;&c_jo;&c_l;&c_ja;&c_k;&c_a;&c_t;&c_a;&c_r;</trd>
  <trdgrp lng="en">
    <trd>radar</trd>,
    <trd>radar set</trd>,
    <trd>radio-locator</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Radar</trd>,
    <trd>Radarger&auml;t</trd>,
    <trd>Funkpeiler</trd>,
    <trd>Funkmssger&auml;t</trd>
  </trdgrp>
  <trd lng="fr">radar</trd>
  <trdgrp lng="it">
    <trd>radar</trd>,
    <trd>radiolocalizzatore</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>radiolokalizator</trd>,
    <trd>radiolokator</trd>,
    <trd>radar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_d;&c_i;&c_o;&c_l;&c_o;&c_k;&c_a;&c_t;&c_o;&c_r;</trd>,
    <trd>&c_r;&c_a;&c_d;&c_i;&c_o;&c_l;&c_o;&c_k;&c_a;&c_c;&c_i;&c_o;&c_n;&c_n;&c_a;&c_ja;
    &c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_d;&c_a;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x96F7;&#x8FBE; <pr>l&eacute;id&aacute;</pr></trd>,
    <trd>&#x96F7;&#x9054; <pr>l&eacute;id&aacute;</pr></trd>,
    <trd>&#x96F7;&#x8FBE;&#x8BBE;&#x5907; <pr>l&eacute;id&aacute;sh&egrave;b&egrave;i</pr></trd>,
    <trd>&#x96F7;&#x9054;&#x8A2D;&#x5099; <pr>l&eacute;id&aacute;sh&egrave;b&egrave;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lokali.ehxo0ilo">
  <kap>e&hcirc;o<tld/>ilo <fnt><bib>PIV2</bib></fnt>, <var><kap>son<tld/>ilo</kap></var></kap>
  <uzo tip="fak">TEK</uzo>
  <uzo tip="fak">BIO</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="lokali.0ilo"><tld lit="L"/>ilo</ref>,
      uzanta reflektitajn sonajn a&ubreve; ultrasonajn
      ondojn (e&hcirc;on), kaj uzata precipe sub akvo. Tiajn organojn
      havas delfenoj kaj vespertoj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_h;&c_a;&c_l;&c_ja;&c_k;&c_a;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_g;&c_ib;&c_d;&c_r;&c_a;&c_l;&c_ja;&c_k;&c_a;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_s;&c_a;&c_n;&c_a;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>sonar</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>akustisches <ind>Ortungsger&auml;t</ind></trd>,
    <trd>Schallger&auml;t</trd>,
    <trd>Schallradar</trd>,
    <trd>Unterwasserortungsanlage<klr> (sub akvo)</klr></trd>,
  </trdgrp>
  <trd lng="fr">sonar</trd>
  <trdgrp lng="it">
    <trd>sonar</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>echolokator</trd>,
    <trd>sonar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_h;&c_o;&c_l;&c_o;&c_k;&c_a;&c_t;&c_o;&c_r;</trd>,
    <trd>&c_s;&c_o;&c_n;&c_a;&c_r;</trd>,
    <trd>&c_g;&c_i;&c_d;&c_r;&c_o;&c_l;&c_o;&c_k;&c_a;&c_t;&c_o;&c_r;
    <klr> (sub akvo)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="lokali.optika0ilo">
  <kap>optika <tld/>ilo, <var><kap>lasera <tld/>ilo</kap></var></kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="lokali.0ilo"><tld lit="L"/>ilo</ref>,
      uzanta reflektitajn elektromagnetajn
      ondojn de optika diapazono (de ultraviolaj &gcirc;is
      infraru&gcirc;aj),
      generatajn de <ref tip="vid" cel="laser.0o">lasero</ref>:
      <ekz>
        MLRS <klr>[estas]</klr> lasera <tld/>ilo uzita kun teleskopo de 0,76&#x202F;m por
        lasera distancmezuro de satelito kaj de Luno
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=389778"
              >Observatorio McDonald</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_p;&c_t;&c_y;&c_ch;&c_n;&c_y; &c_l;&c_ja;&c_k;&c_a;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_l;&c_ja;&c_z;&c_e;&c_r;&c_n;&c_y; &c_l;&c_ja;&c_k;&c_a;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_l;&c_ib;&c_d;&c_a;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>optical <ind>radar</ind></trd>,
    <trd>optical <ind>ranger</ind></trd>,
    <trd>lidar</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Laser-Radar</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>radar ottico</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>namiernik optyczny</trd>,
    <trd>namiernik laserowy</trd>,
    <trd>lokalizator optyczny</trd>,
    <trd>lokalizator laserowy</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_p;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;
    <ind>&c_l;&c_o;&c_k;&c_a;&c_t;&c_o;&c_r;</ind></trd>,
    <trd>&c_l;&c_a;&c_z;&c_je;&c_r;&c_n;&c_y;&c_j;
    <ind>&c_l;&c_o;&c_k;&c_a;&c_t;&c_o;&c_r;</ind></trd>,
    <trd>&c_l;&c_i;&c_d;&c_a;&c_r;</trd>
  </trdgrp>
</drv>
</art>
