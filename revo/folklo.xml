<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: folklo.xml,v 1.27 2022/09/22 08:43:19 revo Exp $">
    <kap>
      <ofc>9</ofc><rad>folklor</rad>/o <fnt><bib>PV</bib></fnt>
    </kap>

    <drv mrk="folklo.0o">
      <kap><ofc>9</ofc><tld/>o</kap>
      <snc mrk="folklo.0o.BELA">
        <uzo tip="fak">BELA</uzo>
	<dif>
	  Popolarto, popola kulturo:
	  <ekz>
	    trovi&gcirc;as en la &scirc;etlanda <tld/>o, mallonga versa&jcirc;o [...] en kiu
	    iu
	    patrino el insulo Unst [...] fieras pri tio, ke &scirc;ia filo lernas la
	    skotan a&ubreve; skotanglan
	    <fnt>
              <aut>G. MacAoidh</aut>,
              <vrk>La &scirc;etlanda...</vrk>,
	      <bib>Monato</bib>,
              <lok>jaro 2001a, numero 11a, p. 14a</lok>
            </fnt>.
	  </ekz>
	</dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_f;&c_a;&c_l;&c_mol;&c_k;&c_l;&c_jo;&c_r;</trd>,
        <trd>&c_n;&c_a;&c_r;&c_o;&c_d;&c_n;&c_a;&c_ja; &c_t;&c_v;&c_o;&c_r;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
      </trdgrp>
      <trd lng="ca">folklore</trd>
      <trdgrp lng="de">
	<trd>Folklore</trd>,
	<trd>Volkskultur</trd>
      </trdgrp>
      <trd lng="en">folklore</trd>
      <trd lng="es">folclore</trd>
      <trdgrp lng="fr">
	<trd>arts et traditions populaires</trd>,
	<trd>folklore</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>folkl&oacute;r</trd>,
        <trd>n&eacute;pm&udblac;v&eacute;szet</trd>
      </trdgrp>
      <trd lng="nl">folklore</trd>
  <trdgrp lng="pt">
    <trd>folclore</trd>,
    <trd>cultura popular</trd>,
    <trd>popul&aacute;rio</trd>
  </trdgrp>
      <trdgrp lng="ru">
	<trd>&c_n;&c_a;&c_r;&c_o;&c_d;&c_n;&c_o;&c_je; &c_t;&c_v;&c_o;&c_r;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>,
	<trd>&c_f;&c_o;&c_l;&c_mol;&c_k;&c_l;&c_o;&c_r;</trd>
      </trdgrp>
      <trdgrp lng="uk">
    <trd>&c_f;&#x43E;&#x43B;&#x44C;&#x43A;&#x43B;&#x43E;&c_r;</trd>,
    <trd>&#x43D;&c_a;&c_r;&#x43E;&c_d;&#x43D;&c_a; &c_t;&c_v;&#x43E;&c_r;&c_ch;&c_ib;&c_s;&c_t;&#x44C;</trd>
  </trdgrp>
</drv>

    <drv mrk="folklo.0a">
      <kap><tld/>a</kap>
      <snc>
	<dif>
	  Popolarta:
	  <ekz>
	    subtenado de minoritataj kulturcentroj, <tld/>aj
	    festivaloj
	    <fnt>
              <aut>S. Brylska</aut>,
              <vrk>&Ccirc;u reiri al naciaj radikoj?</vrk>,
              <bib>Monato</bib>,
              <lok>jaro 1993a, numero 8a, p. 12a</lok>
            </fnt>.
	  </ekz>
	</dif>
      </snc>
  <trd lng="be">&c_f;&c_a;&c_l;&c_mol;&c_k;&c_l;&c_jo;&c_r;&c_n;&c_y;</trd>
  <trd lng="ca">folkl&ograve;ric</trd>
  <trdgrp lng="cs">
    <trd>folklorn&#xED;</trd>,
    <trd>folkl&#xF3;rov&#xFD;</trd>,
    <trd>n&#xE1;rodopisn&#xFD;</trd>
  </trdgrp>
  <trd lng="de">folkloristisch</trd>
  <trd lng="en">folkloric</trd>
  <trdgrp lng="fr">
	<trd>folklorique</trd>
      </trdgrp>
  <trdgrp lng="hu">
        <trd>n&eacute;pm&udblac;v&eacute;szeti</trd>,
        <trd>folkl&oacute;r</trd>
      </trdgrp>
  <trd lng="nl">folkloristisch</trd>
  <trd lng="ru">&c_f;&c_o;&c_l;&c_mol;&c_k;&c_l;&c_o;&c_r;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>folkl&#xF3;rny</trd>,
    <trd>n&#xE1;rodopisn&#xFD;</trd>
  </trdgrp>
  <trd lng="uk">&c_f;&#x43E;&#x43B;&#x44C;&#x43A;&#x43B;&#x43E;&c_r;&#x43D;&c_i;&c_j;</trd>
</drv>

<drv mrk="folklo.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="folklo.0isto.BELA">
    <dif>
      Fakulo pri folkloro, pri popola kulturo:
      <ekz>
        <tld/>isto Wang Zuoji estas tre okupita, &ccirc;ar li devas 
        <klr>...</klr> klarigi al gejunuloj diversajn tradiciajn 
        kutimojn kaj morojn en Printempa Festo
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url ref="&CRI;1/2007/02/15/1@54201.htm">
          Folkloristo Wang Zuoji</url></vrk>,
          <lok>2007-02-15</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_l;&c_mol;&c_k;&c_l;&c_ja;&c_r;&c_y;&c_s;&c_t;</trd>
  <trd lng="ca">folklorista</trd>
  <trdgrp lng="cs">
    <trd>folklorista</trd>,
    <trd>n&#xE1;rodopisec</trd>
  </trdgrp>
  <trd lng="en">folklorist</trd>
  <trd lng="fr">folkloriste</trd>
  <trd lng="sk">n&#xE1;rodopisec</trd>
  <trd lng="uk">&c_f;&#x43E;&#x43B;&#x44C;&#x43A;&#x43B;&#x43E;&c_r;&c_i;&c_s;&c_t;</trd>
</drv>
  </art>
