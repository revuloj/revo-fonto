<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>folklor</rad>/o
    </kap>

    <drv mrk="folklo.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">BELA</uzo>
      <snc>
	<dif>
	  Popolarto:
	  <ekz>
	    trovi&gcirc;as en la &scirc;etlanda <tld/>o, mallonga versa&jcirc;o [...] en kiu iu
	    patrino el insulo Unst [...] fieras pri tio, ke &scirc;ia filo lernas la
	    skotan a&ubreve; skotanglan
	    <fnt><aut>Garbhan MacAoidh</aut>: <vrk>La
		&scirc;etlanda - malaperanta lingva hereda&jcirc;o</vrk>,
	      <bib>Monato</bib><lok>jaro 2001a, numero 11a, p. 14a</lok></fnt>.
	  </ekz>
	</dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_f;&c_a;&c_l;&c_mol;&c_k;&c_l;&c_jo;&c_r;</trd>,
        <trd>&c_n;&c_a;&c_r;&c_o;&c_d;&c_n;&c_a;&c_ja; &c_t;&c_v;&c_o;&c_r;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
      </trdgrp>
      <trd lng="ca">folklore</trd>
      <trdgrp lng="de">
	<trd>Folklore</trd>,
	<trd>Volkskultur</trd>
      </trdgrp>
      <trd lng="en">folklore</trd>
      <trd lng="es">folclore</trd>
      <trdgrp lng="fr">
	<trd>arts et traditions populaires</trd>,
	<trd>folklore</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>folkl&oacute;r</trd>,
        <trd>n&eacute;pm&udblac;v&eacute;szet</trd>
      </trdgrp>
      <trd lng="nl">folklore</trd>
      <trdgrp lng="ru">
	<trd>&c_n;&c_a;&c_r;&c_o;&c_d;&c_n;&c_o;&c_je; &c_t;&c_v;&c_o;&c_r;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>,
	<trd>&c_f;&c_o;&c_l;&c_mol;&c_k;&c_l;&c_o;&c_r;</trd>
      </trdgrp>
    </drv>

    <drv mrk="folklo.0a">
      <kap><tld/>a</kap>
      <snc>
	<dif>
	  Popolarta:
	  <ekz>
	    subtenado de minoritataj kulturcentroj, <tld/>aj
	    festivaloj
	    <fnt><aut>Sylvia Brylska</aut>: <vrk>&Ccirc;u reiri al naciaj
		radikoj?</vrk>, <bib>Monato</bib><lok>jaro 1993a, numero 8a, p.
		12a</lok></fnt>.
	  </ekz>
	</dif>
      </snc>
      <trd lng="be">&c_f;&c_a;&c_l;&c_mol;&c_k;&c_l;&c_jo;&c_r;&c_n;&c_y;</trd>
      <trd lng="de">folkloristisch</trd>
      <trdgrp lng="fr">
	<trd>folklorique</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>n&eacute;pm&udblac;v&eacute;szeti</trd>,
        <trd>folkl&oacute;r</trd>
      </trdgrp>
      <trd lng="nl">folkloristisch</trd>
      <trd lng="ru">&c_f;&c_o;&c_l;&c_mol;&c_k;&c_l;&c_o;&c_r;&c_n;&c_y;&c_j;</trd>
    </drv>
  </art>
