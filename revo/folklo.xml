<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>folklor</rad>/o
</kap>
<drv mrk="folklo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Popolarto:
      <ekz>
        trovi&gcirc;as en la &scirc;etlanda <tld/>o, mallonga versa&jcirc;o [...] en kiu iu
        patrino el insulo Unst [...] fieras pri tio, ke &scirc;ia filo lernas la
        skotan a&ubreve; skotanglan<fnt><aut>Garbhan MacAoidh</aut>: <vrk>La
        &scirc;etlanda - malaperanta lingva hereda&jcirc;o</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 11a, p. 14a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Folklore</trd>,
    <trd>Volkskultur</trd>
  </trdgrp>
  <trd lng="en">folklore</trd>
  <trd lng="es">folklore</trd>
  <trdgrp lng="fr">
    <trd>arts et traditions populaires</trd>,
    <trd>folklore</trd>
  </trdgrp>
  <trd lng="hu">folkl&oacute;r</trd>
  <trd lng="nl">folklore</trd>
  <trd lng="ru">&c_f;&c_o;&c_l;&c_mol;&c_k;&c_l;&c_o;&c_r;</trd>
</drv>

<drv mrk="folklo.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Popolarta:
      <ekz>
        subtenado de minoritataj kulturcentroj, <tld/>aj
        festivaloj<fnt><aut>Sylvia Brylska</aut>: <vrk>&Ccirc;u reiri al naciaj
        radikoj?</vrk>, <bib>Monato</bib><lok>jaro 1993a, numero 8a, p.
        12a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>folklorique</trd>
  </trdgrp>
  <trd lng="nl">folkloristisch</trd>
  </drv>
</art>
