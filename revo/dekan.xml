<art mrk="$Id: dekan.xml,v 1.28 2025/09/17 06:40:42 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>dekan</rad>/o <fnt><bib>JED</bib></fnt>
</kap>

<drv mrk="dekan.0o">
  <kap><tld/>o</kap>
  <snc mrk="dekan.0o.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      &Ccirc;ef<ref tip="super" cel="profes1.0o.PED">profesoro</ref> de universitata fakultato:
      <ekz>
        vic<tld/>o de la Samarkanda medicina instituto
        <fnt>
          <aut>A. Kor&jcirc;enkov</aut>,
          <vrk><url ref="&Sezonoj;2013/03/nekrologo-3/">
          Vladimir Lemelev forpasis</url></vrk>,
          <lok>La Balta Ondo, 2013-03-29</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de Husana teologia fakultato
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#x2116;8-9</lok>
        </fnt>;
      </ekz>
      <ekz>
        Johano <nom>Hus</nom> <klr>[&#x2026;]</klr> estis <tld/>o de la artisma fakultato,
        <klr>[poste]</klr> <ref cel="rektor.0o.universitata">rektoro</ref> de de la praga universitato
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk>Katolikismo: Johano Hus &ndash; &ccirc;u ne plu herezulo?</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="es">decano</trd>
    <trd lng="hu">d&eacute;k&aacute;n</trd>
    <trd lng="pl">dziekan</trd>
    <trd lng="ru">&c_d;&c_je;&c_k;&c_a;&c_n;</trd>
  </snc>
  <snc mrk="dekan.0o.KRI">
  <uzo tip="fak">KRI</uzo>
    <dif>
      Administranto de eklezia distrikto el kelke da paro&hcirc;oj:
      <ekz>
        <tld/>o de la &ccirc;efpre&gcirc;ejo en <nom>Gloucester</nom>
        <fnt>
          <bib>FSp</bib>
          <lok>Unua Parto: Usona Federala Sperto</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="pastr.cxef0o.episkopo">&ccirc;efpastro</ref>
    <trdgrp lng="hu">
      <trd>esperes</trd>,
      <trd>szuperintendens</trd>
    </trdgrp>
    <trd lng="pl">dziekan</trd>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_k;&c_a;&c_n;</trd>
  <trd lng="ca">deg&agrave;</trd>
  <trdgrp lng="cs">
    <trd>d&ecaron;kan fakulty</trd>,
    <trd>vedouc&iacute; fakulty</trd>
  </trdgrp>
  <trd lng="de">Dekan</trd>
  <trd lng="en">dean</trd>
  <trd lng="fr">doyen</trd>
  <trd lng="he">&dalet;&#x5B5;&yod;&qof;&#x5B8;&fnun;</trd>
  <trdgrp lng="ja">
    <trd>&#x5B66;&#x90E8;&#x9577;  <pr>&#x304C;&#x304F;&#x3076;&#x3061;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x4E3B;&#x5E2D;&#x53F8;&#x796D;  <pr>&#x3057;&#x3085;&#x305B;&#x304D;&#x3057;&#x3055;&#x3044;</pr></trd>,
    <trd>&#x30C7;&#x30AB;&#x30F3;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>decano</trd>,
    <trd>de&atilde;o</trd>
  </trdgrp>
  <trd lng="sk">dekan fakulty</trd>
  <trd lng="uk">&c_d;&c_je;&c_k;&c_a;&c_n;</trd>
  <trdgrp lng="zh">
    <trd>&#x7CFB;&#x4E3B;&#x4EFB; <pr>x&igrave;zh&#x1D4;r&egrave;n</pr></trd>,
    <trd>&#x5EA7;&#x5802;&#x7267;&#x5E08; <pr>zu&ograve;t&aacute;ngm&ugrave;sh&imacron;</pr></trd>,
    <trd>&#x9662;&#x9577; <pr>yu&agrave;nzh&#x1CE;ng</pr></trd>
  </trdgrp>
</drv>
</art>
