<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sup.xml,v 1.35 2025/09/08 13:03:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sup</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="sup.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sup.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Flua <ref tip="lst" lst="voko:man&gcirc;a&jcirc;oj" cel="mangx.0ajxo.homa">man&gcirc;a&jcirc;o</ref> konsistanta
      el akvo a&ubreve; lakto kaj diversaj aldona&jcirc;oj (ekz-e legomoj, greno, viandon):
      <ekz>
        <ind>hordea <tld/>o</ind>
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
        <trdgrp lng="de">
          <trd>Graupensuppe</trd>,
          <trd>Gr&auml;upchen</trd>
        </trdgrp>
        <trd lng="id"><baz>sup</baz>sup jelai</trd>
      </ekz>
      <ekz>
        starigu la grandan kaldronon kaj kuiru <tld/>on por la profetidoj
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 4:38</lok>
        </fnt>;
      </ekz>
      <ekz>
        akva <tld/>o, spicita per pipro kaj ranca oleo
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        doni al la malsanuloj <ind>avenan <tld/>on</ind>
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto unua</lok>
        </fnt>;
        <trdgrp lng="id">
          <trd><baz>bubur</baz>bubur haver</trd>,
          <trd><baz>sup</baz>sup haver</trd>,
          <trd>oatmeal</trd>
        </trdgrp>
      </ekz>
      <ekz>
        kamparanoj <klr>[&#8230;]</klr> man&gcirc;is lardan <tld/>on
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Sepa</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi metis <klr>[&#8230;]</klr> spinaco&scirc;inkan <tld/>on
        <fnt>
          <bib>ChC</bib>
          <lok>14</lok>
        </fnt>;
      </ekz>
      <ekz>
        lento-<tld/>o
        <fnt>
          <bib>Kon11</bib>
          <lok>Eksa redaktoro de TEJO Tutmonde Nicola Ruggiero: Mi estas anar&hcirc;iista verdulo kaj
          epikurano</lok>
        </fnt>;
      </ekz>
      <ekz>
        inter la mano &gcirc;is la bu&scirc;o
        ofte disver&scirc;i&gcirc;as la <tld/>o
        <klr>(la esperita&jcirc;o ne efektivi&gcirc;as)
        </klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="brog.0ajxo">broga&jcirc;o</ref>,
      <ref cel="buljon.0o">buljono</ref>,
      <ref cel="pot.unu0ajxo">unupota&jcirc;o</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_p;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_ib;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_u;&c_p;&c_a;</trd>
  <trd lng="bo">&t_tha;&t_shabchu;&t_ga;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trd lng="cs">pol&eacute;vka</trd>
  <trd lng="de">Suppe</trd>
  <trd lng="en">soup</trd>
  <trd lng="es">sopa</trd>
  <trdgrp lng="fr">
    <trd>potage</trd>,
    <trd>soupe</trd>
  </trdgrp>
  <trd lng="he">&mem;&resh;&qof;</trd>
  <trd lng="hr">juha</trd>
  <trd lng="hu">leves</trd>
  <trd lng="id">sup</trd>
  <trd lng="ja">&#12473;&#12540;&#12503;  <pr>&#12377;&#12540;&#12407;</pr></trd>
  <trd lng="nl">soep</trd>
  <trd lng="pl">zupa</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_p;</trd>,
    <trd>&c_p;&c_o;&c_h;&c_l;&c_jo;&c_b;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">polievka</trd>
  <trd lng="sv">soppa</trd>
  <trd lng="tr">&ccedil;orba</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_u;&c_p;</trd>,
    <trd>&c_ju;&c_sh;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#27748;<klr> [t&amacron;ng]</klr></trd>
</drv>

<drv mrk="sup.0ujo">
  <kap>
    <tld/>ujo
  </kap>
  <snc mrk="sup.0ujo.VAZ">
    <dif>
      Ordinare iom dika <ref tip="super" cel="vaz.0o.VAZ">vazo</ref>
      kun kovrilo, en kiu oni tenas <tld/>on:
      <ekz>
        fajencaj <tld/>ujoj kaj pladoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        lasante fali la <tld/>plenan <tld/>ujon
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekdua</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la tablo apud negranda globforma samovaro staris <tld/>ujo kun malvarma
        brasiksupo
        <fnt>
          <aut>A. &Ccirc;e&hcirc;ov, trad. A. Kor&jcirc;enkov</aut>,
          <vrk><url ref="&LOdE;Libroj/V_bibl02.pdf">
          Sveda alumeto</url></vrk>,
          <lok>2004-2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_p;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Suppensch&uuml;ssel</trd>,
    <trd>Terrine</trd>
  </trdgrp>
  <trd lng="en">tureen</trd>
  <trd lng="fr">soupi&egrave;re</trd>
  <trdgrp lng="id">
    <trd><baz>mangkuk</baz>mangkuk besar<klr> (utk sup)</klr></trd>,
    <trd><baz>mangkuk</baz>mangkuk sup</trd>
  </trdgrp>
  <trd lng="ja">&#12473;&#12540;&#12503;&#12400;&#12385;  <pr>&#12377;&#12540;&#12407;&#12400;&#12385;</pr></trd>
  <trd lng="pl">waza</trd>
</drv>
</art>
