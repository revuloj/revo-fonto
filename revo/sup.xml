<art mrk="$Id: sup.xml,v 1.28 2023/09/04 15:14:33 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sup</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sup.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sup.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;a&jcirc;o konsistanta el akvo,
      en kiu oni kuiris legomojn kaj anka&ubreve; kelkfoje viandon:
      <ekz>
        inter la mano &gcirc;is la bu&scirc;o
        ofte disver&scirc;i&gcirc;as la <tld/>o
        <klr>(la esperita&jcirc;o ne efektivi&gcirc;as.
        <refgrp tip="vid">
          <ref cel="spac.inter0o">interspaco</ref>,
          <ref cel="fru">tro frue</ref>
        </refgrp>)
        </klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_p;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_ib;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_u;&c_p;&c_a;</trd>
  <trd lng="bo">&t_tha;&t_shabchu;&t_ga;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trd lng="cs">pol&eacute;vka</trd>
  <trd lng="de">Suppe</trd>
  <trd lng="en">soup</trd>
  <trd lng="es">sopa</trd>
  <trdgrp lng="fr">
    <trd>potage</trd>,
    <trd>soupe</trd>
  </trdgrp>
  <trd lng="he">&mem;&resh;&qof;</trd>
  <trd lng="hr">juha</trd>
  <trd lng="hu">leves</trd>
  <trd lng="id">sup</trd>
  <trd lng="ja">&#x30B9;&#x30FC;&#x30D7;  [<ind>&#x3059;&#x30FC;&#x3077;</ind>]</trd>
  <trd lng="nl">soep</trd>
  <trd lng="pl">zupa</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_p;</trd>,
    <trd>&c_p;&c_o;&c_h;&c_l;&c_jo;&c_b;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">polievka</trd>
  <trd lng="sv">soppa</trd>
  <trd lng="tr">&ccedil;orba</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_u;&c_p;</trd>,
    <trd>&c_ju;&c_sh;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#x6C64;<klr> [t&#x101;ng]</klr></trd>
</drv>

<drv mrk="sup.0ujo">
  <kap>
    <tld/>ujo
  </kap>
  <snc mrk="sup.0ujo.VAZ">
    <dif>
      Ordinare iom dika <ref tip="super" cel="vaz.0o.VAZ">vazo</ref>
      kun kovrilo, en kiu oni tenas <tld/>on:
      <ekz>
        sur la tablo apud negranda globforma samovaro staris <tld/>ujo kun malvarma
        brasiksupo
        <fnt>
          <aut>A. &Ccirc;e&hcirc;ov, trad. A. Kor&jcirc;enkov</aut>,
          <vrk><url ref="&LOdE;Libroj/V_bibl02.pdf">
          Sveda alumeto</url></vrk>,
          <lok>2004-2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">soupi&egrave;re</trd>
  <trd lng="ja">&#x30B9;&#x30FC;&#x30D7;&#x3070;&#x3061;  [<ind>&#x3059;&#x30FC;&#x3077;&#x3070;&#x3061;</ind>]</trd>
</drv>
</art>
