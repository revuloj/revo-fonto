<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kardioid</rad>/o
</kap>
<drv mrk="kardio.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 34</lok></fnt>
  <snc>
    <dif>
      Ebena <ref tip="super" cel="kurb.0o.MAT">kurbo</ref>, naskita de
      punkto
      apartenanta al cirklo, kiu ruli&gcirc;as sur la ekstera periferio de
      alia samradiusa cirklo:
      <ekz>
        <tld/>o estas speco de <ref cel="epicik.0o">epicikloido</ref>;
      </ekz>
      <ekz>
        la polusa ekvacio de <tld/>o estas de la tipo
        <frm>&rho; = 2<k>a</k>(1-cos&theta;)</frm>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/k_kardioido.svg.gif" alt="300" lrg="300">
    <tld/>o
  </bld>
  <trd lng="be">&c_k;&c_a;&c_r;&c_d;&c_y;&c_jo;&c_ib;&c_d;&c_a;</trd>
  <trd lng="bg">&c_k;&c_a;&c_r;&c_d;&c_i;&c_o;&c_i;&c_d;&c_a;</trd>
  <trd lng="ca">cardioide</trd>
  <trd lng="de">Kardioide</trd>
  <trd lng="en">cardioid</trd>
  <trd lng="fr">cardio&iuml;de</trd>
  <trdgrp lng="hu">
    <trd>sz&iacute;vg&ouml;rbe</trd>,
    <trd>kardioid</trd>
  </trdgrp>
  <trd lng="nl">cardio&iuml;de</trd>
  <trd lng="pl">kardioida</trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_d;&c_i;&c_o;&c_i;&c_d;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>kardioida</trd>,
    <trd>srdcovka</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kardioida</trd>,
    <trd>srdcovka</trd>
  </trdgrp>
</drv>
</art>

