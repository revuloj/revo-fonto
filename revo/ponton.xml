<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ponton.xml,v 1.23 2024/02/17 07:52:34 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>ponton</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ponton.0o">
  <kap><tld/>o</kap>
  <snc mrk="ponton.0o.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      Platfunda &scirc;ipo, uzata por fari flosponton.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_n;&c_t;&c_o;&c_n;</trd>
  <trd lng="cs">ponton</trd>
  <trd lng="de">Ponton</trd>
  <trd lng="en">pontoon</trd>
  <trd lng="es">pont&oacute;n<klr> (embarcaci&oacute;n)</klr></trd>
  <trd lng="fr">ponton</trd>
  <trd lng="hu">ponton</trd>
  <trdgrp lng="ja">
    <trd>&#x5E73;&#x5E95;&#x821F;  <pr>&#x3072;&#x3089;&#x305D;&#x3053;&#x3075;&#x306D;</pr></trd>,
    <trd>&#x8239;&#x6A4B;&#x7528;&#x7BB1;&#x821F;  <pr>&#x3075;&#x306A;&#x3070;&#x3057;&#x3088;&#x3046;&#x306F;&#x3053;&#x3075;&#x306D;</pr></trd>,
    <trd>&#x5EC3;&#x8239;  <pr>&#x306F;&#x3044;&#x305B;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">ponton</trd>
  <trd lng="pl">ponton</trd>
  <trd lng="pt">pont&atilde;o</trd>
  <trd lng="ro">ponton</trd>
  <trd lng="ru">&c_p;&c_o;&c_n;&c_t;&c_o;&c_n;</trd>
  <trd lng="sk">pont&#xF3;n</trd>
  <trd lng="sv">ponton</trd>
  <trd lng="uk">&#x43F;&#x43E;&#x43D;&c_t;&#x43E;&#x43D;&#x43D;&c_i;&c_j; &#x43C;&c_ib;&c_s;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#x6D6E;&#x6865; <pr>f&#xFA;qi&#xE1;o</pr></trd>,
    <trd>&#x6D6E;&#x6A4B; <pr>f&#xFA;qi&#xE1;o</pr></trd>,
    <trd>&#x8DB8;&#x8239; <pr>d&#x1D4;nchu&#xE1;n</pr></trd>,
    <trd>&#x8E89;&#x8239; <pr>d&#x1D4;nchu&#xE1;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ponton.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="ponton.0isto.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Soldato de milita in&gcirc;enierado, kiu uzas <tld/>ojn,
      konstruas provizorajn pontojn kaj simile:
      <ekz>
        la oficiroj de la <tld/>istoj
        <fnt>
          <aut>Erckmann-Chatrian, trad. P. J. Doumen</aut>,
          <vrk><url ref="http://esperanto-forum.net/doc/Romanoj/konskripto1813.pdf">
          Soldato de Napoleono</url></vrk>,
          <lok>[vidita en 2015]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_n;&c_t;&c_o;&c_n;&c_sh;&c_ch;&c_y;&c_k;</trd>
  <trd lng="fr">pontonnier<klr> (mil.)</klr></trd>
  <trd lng="pl">pontonier</trd>
  <trd lng="ro">pontonier&abreve;</trd>
</drv>
</art>
