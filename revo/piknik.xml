<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>piknik</rad>/o
</kap>
<drv mrk="piknik.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Eksterurba plezura ekskurso a&ubreve; promenado, dum kiu oni senceremonie
      komune man&gcirc;as en plena aero.
    </dif>
  </snc>
  <trd lng="en">picnic</trd>
  <trd lng="nl">picknick</trd>
  <trd lng="ru">&c_p;&c_i;&c_k;&c_n;&c_i;&c_k;</trd>
</drv>
<drv mrk="piknik.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Fari tian ekskurson kaj man&gcirc;on.
    </dif>
  </snc>
  <trd lng="en">picnic</trd>
  <trd lng="nl">picknicken</trd>
</drv>
</art>

