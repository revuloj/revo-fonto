<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>ambulanc</rad>/o
</kap>
<drv mrk="ambula.0o">
  <kap><tld/>o</kap>
  <snc mrk="ambula.0o.vunditejo">
    <dif>
      Movebla milithospitalo; provizora vunditejo.
    </dif>
    <trd lng="de">Feldlazarett</trd>
    <trdgrp lng="en">
      <trd>field hospital</trd>,
      <trd>M.A.S.H.</trd>
    </trdgrp>
    <trd lng="fr">h&ocirc;pital de campagne</trd>
    <trd lng="nl">veldhospitaal</trd>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_je;&c_v;&c_o;&c_j; &c_g;&c_o;&c_s;&c_p;&c_i;&c_t;&c_a;&c_l;&c_mol;</trd>
  </snc>
  <snc mrk="ambula.0o.veturilo">
    <dif>
      Veturilo, ekipita por transporti kaj flegi vunditojn a&ubreve; malsanulojn.
    </dif>
    <trdgrp lng="de">
      <trd>Krankenwagen</trd>,
      <trd>Krankentransportwagen</trd>,
      <trd>Krankenauto</trd>,
      <trd>Ambulanz</trd>
    </trdgrp>
    <trd lng="en">ambulance</trd>
    <trd lng="es">ambulancia</trd>
    <trd lng="fr">ambulance</trd>
    <trdgrp lng="nl">
      <trd>ziekenauto</trd>,
      <trd>ziekenwagen</trd>,
      <trd>ambulance</trd>
    </trdgrp>
    <trd lng="ru">&c_m;&c_a;&c_sh;&c_i;&c_n;&c_a; <ind>&c_s;&c_k;&c_o;&c_r;&c_o;&c_j; &c_p;&c_o;&c_m;&c_o;&c_shch;&c_i;</ind></trd>
    <trd lng="sk">sanitka</trd>
  </snc>
</drv>
</art>
