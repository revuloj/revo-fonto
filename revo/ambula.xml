<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>ambulanc</rad>/o
</kap>
<drv mrk="ambula.0o">
  <kap><tld/>o</kap>
  <snc mrk="ambula.0o.veturilo">
    <dif>
      Veturilo, ekipita por transporti kaj flegi vunditojn a&ubreve;
      malsanulojn:
      <ekz>
        en Germanio <tld/>o atingas pacienton post meze ok &gcirc;is 14
        minutoj<fnt><aut>Jomo Ipfelkofer</aut>: <vrk>Esploroj
        kortu&scirc;aj</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 9a, p.
        12a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_a;&c_sh;&c_y;&c_n;&c_a; &c_h;&c_u;&c_t;&c_k;&c_a;&c_j; &c_d;&c_a;&c_p;&c_a;&c_m;&c_o;&c_g;&c_ib;</trd>
    <trdgrp lng="de">
      <trd>Krankenwagen</trd>,
      <trd>Krankentransportwagen</trd>,
      <trd>Krankenauto</trd>,
      <trd>Ambulanz</trd>
    </trdgrp>
    <trd lng="en">ambulance</trd>
    <trd lng="es">ambulancia</trd>
    <trd lng="fr">ambulance</trd>
    <trd lng="it">ambulanza</trd>
    <trdgrp lng="nl">
      <trd>ziekenauto</trd>,
      <trd>ziekenwagen</trd>,
      <trd>ambulance</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>karetka</trd>,
      <trd>ambulans</trd>
    </trdgrp>
    <trd lng="pt">ambul&acirc;ncia</trd>
    <trd lng="ru">&c_m;&c_a;&c_sh;&c_i;&c_n;&c_a; <ind>&c_s;&c_k;&c_o;&c_r;&c_o;&c_j;
    &c_p;&c_o;&c_m;&c_o;&c_shch;&c_i;</ind></trd>
    <trd lng="sk">sanitka</trd>
  </snc>
  <snc mrk="ambula.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Movebla milithospitalo; provizora vunditejo.
    </dif>
    <trd lng="be">&c_p;&c_a;&c_l;&c_ja;&c_v;&c_y; &c_sh;&c_p;&c_ib;&c_t;&c_a;&c_l;&c_mol;</trd>
    <trd lng="de">Feldlazarett</trd>
    <trdgrp lng="en">
      <trd>field hospital</trd>,
      <trd>M.A.S.H.</trd>
    </trdgrp>
    <trd lng="fr">h&ocirc;pital de campagne</trd>
    <trd lng="it"><ind>ospedale</ind> da campo</trd>
    <trd lng="nl">veldhospitaal</trd>
    <trd lng="pl">szpital polowy</trd>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_je;&c_v;&c_o;&c_j; &c_g;&c_o;&c_s;&c_p;&c_i;&c_t;&c_a;&c_l;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>ambulance</trd>,
    <trd>pojízdná nemocnice</trd>,
    <trd>sanita</trd>,
    <trd>sanitka</trd>,
    <trd>sanitní auto</trd>,
    <trd>záchranka</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pojazdná vojenská nemocnica</trd>,
    <trd>záchranka</trd>
  </trdgrp>
</drv>
</art>
