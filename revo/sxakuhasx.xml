<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&scirc;akuha&ccirc;</rad>/o <fnt><bib>JapEnc</bib></fnt>
</kap>
<drv mrk="sxakuhasx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Japana klasika <ref tip="super"
cel="instru2.blov0o">blovinstrumento</ref>
      kun milda, densa sono,
      farita el 7-noda, proksimume 55 cm longa bambupeco,
      kun 4 truoj fronte kaj unu dorse,
      rekte tenata, kun beko tegita per bufala korno.
    </dif>
  </snc>
  <trd lng="de">Shakuhachi-Fl&ouml;te</trd>
  <trdgrp lng="fr">
    <trd>shakuhachi</trd>
  </trdgrp>
