<drv mrk="sercx.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Per <tld/>o
      <ref tip="super" cel="trov.0i.elsercxi">trovi<sncref/></ref>
      difinita(j)n inter iuj:
      <ekz>
        Faraono el<tld/>u homon kompetentan kaj sa&gcirc;an
        kaj estrigu lin super la Egipta lando
	<fnt><bib>MT</bib>, <lok>&Gen; 41:33</lok></fnt>;
      </ekz>
      <ekz>
        el<tld/>u inter la tuta popolo homojn bravajn, veramajn,
        neprofitamajn
        <fnt><bib>MT</bib>, <lok>&Eli; 18:21</lok></fnt>;
      </ekz>
      <ekz>
        el<tld/>u al vi mem unu el la plej belaj floroj, kiuj kreskas tie
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> el<tld/>is en la arbaro la lokon, kie staris la tilio
        <fnt>
          <bib>Fab2</bib>
          <lok>Elfo de la rozo</lok>
        </fnt>;
      </ekz>
      <ekz>
        el<tld/>i vojon al Hindujo
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> eniris en la salonon, por el<tld/>i la kelneron
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni el<tld/>is kelke da altaj arboj, kiuj kreskis apud la marbordo, por ke ni
        povu surna&gcirc;igi mian boaton
        <fnt>
          <bib>Rob</bib>
          <lok>Parto IV</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="elekt.0i.KOMUNE">elekti<sncref/></ref>,
      <ref cel="rigard.el0i.elelekti">elrigardi<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_sh;&c_u;&c_k;&c_v;&c_a;&c_c;&c_mol;</trd>
