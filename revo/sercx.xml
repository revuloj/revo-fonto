<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ser&ccirc;</rad>/i
</kap>

<drv mrk="sercx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Peni por trovi:
      <ekz>
        mi kondukos vin al tiu, kiun vi <tld/>as<fnt>Z</fnt>;
      </ekz><ekz>
        mi <tld/>as mian horlo&gcirc;on, sed ne trovas &gcirc;in;
      </ekz><ekz>
        <tld/>i ion per la okuloj<fnt>Z</fnt>;
      </ekz><ekz>
        <tld/>i rimedon por;
      </ekz><ekz>
        <tld/>i al si plezuron<fnt>Z</fnt>;
      </ekz><ekz>
        <tld/>i instruon<fnt>Z</fnt>,
      </ekz><ekz>
        amikecon<fnt>Z</fnt>,
      </ekz><ekz>
        honoron<fnt>Z</fnt>,
      </ekz><ekz>
        scion<fnt>Z</fnt>,
      </ekz><ekz>
        subtenon<fnt>Z</fnt>;
      </ekz><ekz>
        <tld/>i sian gloron<fnt>Z</fnt>;
      </ekz><ekz>
        <tld/>i favoron de eminentulo<fnt>Z</fnt>;
      </ekz><ekz>
        <tld/>i la sa&gcirc;econ<fnt>Z</fnt>;
      </ekz><ekz>
        <tld/>i pereon<fnt>Z</fnt>;
      </ekz><ekz>
        fi&scirc;o <tld/>as profundon, homo abundon<fnt>Z</fnt>;
      </ekz><ekz>
        <tld/>i la &gcirc;ustan esprimon;
      </ekz><ekz>
        li <tld/>as venton sur la kampo<fnt>Z</fnt>,
      </ekz><ekz>
        la tagon pasintan<fnt>Z</fnt>
        <klr>(netrovebla&jcirc;on)</klr>;
      </ekz><ekz>
        el<tld/>u inter la tuta popolo homojn bravajn, veramajn,
        neprofitamajn<fnt>Z</fnt>.
      </ekz> 
    </dif>
    <refgrp tip="sin">
      <ref cel="esplor.0i">esplori</ref>,
      <ref cel="fos.0i">fosi</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">hledat</trd>
  <trd lng="de">suchen</trd>
  <trd lng="en">search</trd>
  <trd lng="es">buscar</trd>
  <trd lng="fr">chercher</trd>
  <trd lng="it">cercare</trd>
  <trd lng="nl">zoeken</trd>
  <trd lng="ru">&c_i;&c_s;&c_k;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="sercx.0ateco">
  <kap><tld/>ateco</kap>
  <snc>
    <dif>
      Stato de persono, kun kiu &ccirc;iuj tre deziras interrilati:
      <ekz>
        li &gcirc;uas <tld/>atecon<fnt>Z</fnt>;
      </ekz><ekz>
        &scirc;i devas ellabori al si nomon, famon, sekve
        <tld/>atecon<fnt>Z</fnt>.
      </ekz> 
    </dif>
  </snc>
</drv>

<drv mrk="sercx.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="S"/>i en la vestoj de iu, por vidi, &ccirc;u li ion ka&scirc;as:
      <ekz>
        la doganistoj pri<tld/>is min.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">durchsuchen</trd>
  <trd lng="nl">fouilleren</trd>
  <trd lng="ru">&c_o;&c_b;&c_y;&c_s;&c_k;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="sercx.tra0i">
  <kap>tra<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Zorge kaj detale <tld/>i:
      <ekz>
        la policistoj tra<tld/>is lian lo&gcirc;ejon.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">durchsuchen</trd>
  <trd lng="nl">doorzoeken</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_y;&c_s;&c_k;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_ch;&c_je;&c_s;&c_a;&c_t;&c_mol;</trd></trdgrp>
</drv>

<drv mrk="sercx.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Rimedo por <tld/>i:
      <ekz>
        <uzo tip="fak">MIL</uzo>
        <ind>min<tld/>ilo</ind>;
        <trd lng="en">mine-detector</trd>
        <trd lng="ru">&c_m;&c_i;&c_n;&c_o;&c_i;&c_s;&c_k;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
      </ekz><ekz>
        <uzo tip="fak">KOMP</uzo>
        <ind>erar<tld/>ilo</ind>;
        <trd lng="en">debugger</trd>
        <trd lng="ru">&c_o;&c_t;&c_l;&c_a;&c_d;&c_ch;&c_i;&c_k;</trd>
      </ekz><ekz>
        <uzo tip="fak">FOT</uzo>
        <ind>vido<tld/>ilo</ind>
        <klr>(fotografia <ref tip="sin" cel="cel.0ilo">celilo</ref>)</klr>;
        <trd lng="en">viewfinder</trd>
        <trd lng="ru">&c_v;&c_i;&c_d;&c_o;&c_o;&c_i;&c_s;&c_k;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
      </ekz>
    </dif>
    <subsnc mrk="sercx.0ilo.komando">
      <uzo tip="fak">KOMP</uzo>
      <dif>
        Komando de redaktilo ebliganta trovi bezonatan
        tekstopecon.
      </dif>
      <trd lng="en">search command</trd>
      <trd lng="ru">&c_k;&c_o;&c_m;&c_a;&c_n;&c_d;&c_a; &c_p;&c_o;&c_i;&c_s;&c_k;&c_a;</trd>
    </subsnc>
    <subsnc mrk="sercx.0ilo.programo">
      <uzo tip="fak">KOMP</uzo>
      <dif>
        Programo (eventuale, rulata per servilo) por ser&ccirc;i
        informojn en datumbazo a&ubreve; en la Interreto.
      </dif>
      <trd lng="en">search engine</trd>
      <trd lng="pl">przeszukiwarka</trd>
      <trd lng="ru">&c_s;&c_i;&c_s;&c_t;&c_je;&c_m;&c_a; &c_p;&c_o;&c_i;&c_s;&c_k;&c_a; &c_i;&c_n;&c_f;&c_o;&c_r;&c_m;&c_a;&c_c;&c_i;&c_i;</trd>
      <trd lng="tr">arama motor&inodot;</trd>
    </subsnc>
  </snc>
  <trd lng="en">finder</trd>
  <trd lng="ru">&c_i;&c_s;&c_k;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
</drv>

</art>
