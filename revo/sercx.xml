<drv mrk="sercx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      La ago <tld/>i:
      <ekz>
        &ccirc;iuj <tld/>oj en la da&ubreve;ro de du tagoj estis vanaj
	<fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;ap. 14-a</lok></fnt>;
      </ekz>
      <ekz>
        post mallonga &ccirc;irka&ubreve;rigardo la infanoj trovis la objekton de la <tld/>o
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        li deprenis <klr>[&#x2026;]</klr> malgrandan libron kaj, post kelka <tld/>o, trovis la
        lokon
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> ricevas novajn eblecojn de laboro, kio rezultigas la <tld/>on
        de pluaj novaj esprimoj
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o montri&gcirc;is frukta, inter la malmultaj dokumentoj trovi&gcirc;is anonima
        letero
        <fnt>
          <bib>ChB</bib>
          <lok>12</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o per la metodo de provoj kaj eraroj
        <fnt>
          <bib>KFl</bib>
          <lok>I. Enkonduko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o pri perfekta partnero
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="https://www.monato.be/2005/008601.php">Torturado televida</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de ekvilibro inter konkurado kaj tolera kunvivo
        <fnt>
          <bib>Monato</bib>
          <aut>Zlatko Ti&scaron;ljar</aut>
          <vrk><url ref="https://www.monato.be/2009/009783.php">Kiel eliri el la krizo?</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o pri ekstertera vivo
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="https://www.monato.be/2013/011081.php">Astrobiologio kaj
          religio</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o per <nom>Google</nom> informis min <klr>[&#x2026;]</klr> pri &ccirc;i tiu libro
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="https://www.monato.be/2008/009402.php">Socipolitika satiro</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o-rezultoj povas almena&ubreve; doni materialon por plua pripensado
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#x2116;5</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de funkcianta necesejo i&gcirc;is serioza problemo
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 8-9</lok>
        </fnt>;
      </ekz>
      <ekz>
        li pledis por <tld/>o de racia solvo de la problemo
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 8-9 (106-107)</lok>
        </fnt>;
      </ekz>
      <ekz>
        inter la IIK-programeroj estis <klr>[&#x2026;]</klr> sportaj olimpikoj kun premioj,
        apudlaga fajro, du trezor-<tld/>oj, muzikado
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 10 (108)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
