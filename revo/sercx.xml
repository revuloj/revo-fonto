<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>ser&ccirc;</rad>/i
    </kap>

    <drv mrk="sercx.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Peni por <ref cel="trov.0i.elsercxi">trovi<sncref/></ref>:
          <ekz>
            mi kondukos vin al tiu, kiun vi <tld/>as<fnt>Z</fnt>;
          </ekz><ekz>
            mi <tld/>as mian horlo&gcirc;on, sed ne trovas &gcirc;in;
          </ekz><ekz>
            <tld/>i ion per la okuloj<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>i rimedon por;
          </ekz><ekz>
            <tld/>i al si plezuron<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>i instruon<fnt>Z</fnt>,
          </ekz><ekz>
            amikecon<fnt>Z</fnt>,
          </ekz><ekz>
            honoron<fnt>Z</fnt>,
          </ekz><ekz>
            scion<fnt>Z</fnt>,
          </ekz><ekz>
            subtenon<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>i sian gloron<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>i favoron de eminentulo<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>i la sa&gcirc;econ<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>i pereon<fnt>Z</fnt>;
          </ekz><ekz>
            fi&scirc;o <tld/>as profundon, homo abundon<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>i la &gcirc;ustan esprimon;
          </ekz><ekz>
            li <tld/>as venton sur la kampo<fnt>Z</fnt>,
          </ekz><ekz>
            la tagon pasintan<fnt>Z</fnt>
            <klr>(netrovebla&jcirc;on)</klr>;
          </ekz> 
        </dif>
        <refgrp tip="sin">
          <ref cel="esplor.0i">esplori</ref>,
          <ref cel="fos.0i">fosi</ref>.
        </refgrp>
      </snc>
      <trd lng="be">&c_sh;&c_u;&c_k;&c_a;&c_c;&c_mol;</trd>
      <trdgrp lng="ca">
        <trd>buscar</trd>,
        <trd>cercar</trd>
      </trdgrp>
      <trd lng="cs">hledat</trd>
      <trd lng="de">suchen</trd>
      <trd lng="en">search</trd>
      <trd lng="es">buscar</trd>
      <trd lng="fr">chercher</trd>
      <trd lng="hu">keres</trd>
      <trd lng="it">cercare</trd>
      <trd lng="nl">zoeken</trd>
      <trd lng="oc">cercar</trd>
      <trd lng="ru">&c_i;&c_s;&c_k;&c_a;&c_t;&c_mol;</trd>
      <trdgrp lng="sv">
        <trd>s&ouml;ka</trd>,
        <trd>leta</trd>
      </trdgrp>
    </drv>

    <drv mrk="sercx.0ateco">
      <kap><tld/>ateco</kap>
      <snc>
        <dif>
          Stato de persono, kun kiu &ccirc;iuj tre deziras interrilati:
          <ekz>
            li &gcirc;uas <tld/>atecon<fnt>Z</fnt>;
          </ekz><ekz>
            &scirc;i devas ellabori al si nomon, famon, sekve
            <tld/>atecon<fnt>Z</fnt>.
          </ekz> 
        </dif>
      </snc>
      <trd lng="hu">keresetts&eacute;g</trd>
      <trd
      lng="ru">&c_v;&c_o;&c_s;&c_t;&c_r;&c_je;&c_b;&c_o;&c_v;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
    </drv>

    <drv mrk="sercx.el0i">
      <kap>el<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Per <tld/>o
          <ref tip="super" cel="trov.0i.elsercxi">trovi<sncref/></ref>:
          <ekz>
            kaj nun Faraono el<tld/>u homon kompetentan kaj sa&gcirc;an
            kaj estrigu lin super la Egipta lando
	    <fnt><bib>MT</bib>, <lok>&Gen; 41:33</lok></fnt>;
          </ekz><ekz>
            el<tld/>u inter la tuta popolo homojn bravajn, veramajn,
            neprofitamajn<fnt><bib>MT</bib>, <lok>&Eli;
            18:21</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="es">encontrar</trd>
      <trdgrp lng="ru">
        <trd>&c_i;&c_z;&c_y;&c_s;&c_k;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_v;&c_y;&c_i;&c_s;&c_k;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
    </drv>

    <drv mrk="sercx.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          La ago <tld/>i:
          <ekz>
            &Ccirc;iuj <tld/>oj en la da&ubreve;ro de du tagoj estis vanaj
	    <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;ap. 14-a</lok></fnt>;
          </ekz><ekz>
            <uzo tip="fak">KOMP</uzo>
             el&ccirc;erpa <tld/>o.
          </ekz>
        </dif>
      </snc>
      <trd lng="en">search</trd>
      <trd lng="ru">&c_p;&c_o;&c_i;&c_s;&c_k;</trd>
    </drv>

    <drv mrk="sercx.pri0i">
      <kap>pri<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          <tld lit="S"/>i en la vestoj de iu, por vidi, &ccirc;u li ion
          ka&scirc;as:
          <ekz>
            la doganistoj pri<tld/>is min.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_a;&c_b;&c_sh;&c_u;&c_k;&c_a;&c_c;&c_mol;</trd>
      <trd lng="de">durchsuchen</trd>
      <trd lng="es">cachear</trd>
      <trd lng="hu">megmotoz</trd>
      <trd lng="nl">fouilleren</trd>
      <trd lng="ru">&c_o;&c_b;&c_y;&c_s;&c_k;&c_a;&c_t;&c_mol;</trd>
      <trd lng="sv">visitera</trd>
    </drv>

    <drv mrk="sercx.tra0i">
      <kap>tra<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Zorge kaj detale <tld/>i:
          <ekz>
            la policistoj tra<tld/>is lian lo&gcirc;ejon.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_a;&c_b;&c_sh;&c_u;&c_k;&c_a;&c_c;&c_mol;</trd>, 
        <trd>&c_p;&c_r;&c_a;&c_ch;&c_a;&c_s;&c_a;&c_c;&c_mol;</trd>
      </trdgrp>
      <trd lng="de">durchsuchen</trd>
      <trd lng="es">registrar</trd>
      <trd lng="hu">&aacute;tkutat</trd>
      <trd lng="nl">doorzoeken</trd>
      <trdgrp lng="ru">
        <trd>&c_o;&c_b;&c_y;&c_s;&c_k;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_p;&c_r;&c_o;&c_ch;&c_je;&c_s;&c_a;&c_t;&c_mol;</trd></trdgrp>
      <trd lng="sv">genoms&ouml;ka</trd>
    </drv>

    <drv mrk="sercx.0ilo">
      <kap><tld/>ilo</kap>
      <snc>
        <dif>
          Rimedo por <tld/>i:
          <ekz>
            <uzo tip="fak">MIL</uzo>
            <ind>min<tld/>ilo</ind>;
            <trd
            lng="be">&c_m;&c_ib;&c_n;&c_a;&c_sh;&c_u;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
            <trd lng="en">mine-detector</trd>
            <trd lng="hu">aknakeres&odblac;</trd>
            <trd
            lng="ru">&c_m;&c_i;&c_n;&c_o;&c_i;&c_s;&c_k;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
          </ekz><ekz>
            <uzo tip="fak">KOMP</uzo>
            <ind>erar<tld/>ilo</ind>;
            <trd lng="en">debugger</trd>
            <trd lng="hu">hibakeres&odblac;</trd>
            <trd
            lng="ru">&c_o;&c_t;&c_l;&c_a;&c_d;&c_ch;&c_i;&c_k;</trd>
          </ekz><ekz>
            <uzo tip="fak">FOT</uzo>
            <ind>vido<tld/>ilo</ind>
            <klr>(fotografia <ref tip="sin"
                cel="cel.0ilo">celilo</ref>)</klr>;
            <trd
            lng="be">&c_v;&c_ib;&c_d;&c_a;&c_sh;&c_u;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
            <trd lng="en">viewfinder</trd>
            <trd lng="hu">keres&odblac;</trd>
            <trd
            lng="ru">&c_v;&c_i;&c_d;&c_o;&c_i;&c_s;&c_k;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
          </ekz>
        </dif>
        <subsnc mrk="sercx.0ilo.komando">
          <uzo tip="fak">KOMP</uzo>
          <dif>
            Komando de redaktilo ebliganta trovi bezonatan
            tekstopecon.
          </dif>
          <trd lng="en">search command</trd>
          <trdgrp lng="es">
            <trd>orden de b&uacute;squda</trd>,
            <trd>comando de b&uacute;squeda</trd>
          </trdgrp>
          <trd lng="hu">keres&odblac;<klr tip="ind">
          (parancs)</klr></trd>
          <trd lng="ru">&c_k;&c_o;&c_m;&c_a;&c_n;&c_d;&c_a;
          &c_p;&c_o;&c_i;&c_s;&c_k;&c_a;</trd>
        </subsnc>
        <subsnc mrk="sercx.0ilo.programo">
          <uzo tip="fak">KOMP</uzo>
          <dif>
            Programo (eventuale, rulata per servilo) por ser&ccirc;i
            informojn en datumbazo a&ubreve; en la Interreto.
          </dif>
          <trd
          lng="be">&c_p;&c_o;&c_sh;&c_u;&c_k;&c_a;&c_v;&c_ib;&c_k;</trd>
          <trd lng="en">search engine</trd>
          <trdgrp lng="es">
            <trd>motor de b&uacute;squeda</trd>,
            <trd>buscador</trd>
          </trdgrp>
          <trd lng="hu">keres&odblac;g&eacute;p</trd>
          <trd lng="pl">przeszukiwarka</trd>
          <trd lng="ru">&c_s;&c_i;&c_s;&c_t;&c_je;&c_m;&c_a; &c_p;&c_o;&c_i;&c_s;&c_k;&c_a; &c_i;&c_n;&c_f;&c_o;&c_r;&c_m;&c_a;&c_c;&c_i;&c_i;</trd>
          <trd lng="sv">s&ouml;kmotor</trd>
          <trd lng="tr">arama motor&inodot;</trd>
        </subsnc>
      </snc>
      <trdgrp lng="be">
        <trd>&c_p;&c_o;&c_sh;&c_u;&c_k;&c_a;&c_v;&c_ib;&c_k;</trd>, 
        <trd>&c_sh;&c_u;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
      </trdgrp>
      <trd lng="en">finder</trd>
      <trd lng="es">herramienta de b&uacute;squeda</trd>
      <trd lng="ru">&c_i;&c_s;&c_k;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
    </drv>

    <drv mrk="sercx.elcxerpa0o">
      <kap>el&ccirc;erpa <tld/>o
          <fnt><bib>KompLeks</bib></fnt>
          <fnt><bib>PIV2</bib></fnt>
      </kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Metodo solvi problemon per la&ubreve;vica provado de &ccirc;iuj ebloj:
          <ekz>
            kontraste al rikuro, rekursio ebligas uzon de
            el&ccirc;erpa <tld/>o;
          </ekz><ekz>
            Estas problemoj, por kiuj ne ekzistas algoritmo pli
            rendimenta ol el&ccirc;erpa
            <tld/>o <fnt><bib>KompLeks</bib></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="en">exhaustive search</trd>
      <trd lng="ru">&c_p;&c_o;&c_l;&c_n;&c_y;&c_j; &c_p;&c_je;&c_r;&c_je;&c_b;&c_o;&c_r;</trd>
    </drv>

  </art>
