<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pogacx.xml,v 1.5 2024/02/18 18:24:02 revo Exp $">
<kap>
  <rad>poga&ccirc;</rad>/o
</kap>
<drv mrk="pogacx.0o">
  <kap><tld/>o</kap>
  <snc mrk="pogacx.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Diskoforma pan<ref tip="lst" lst="voko:man&gcirc;a&jcirc;oj" cel="bak.0ajxo.KUI">baka&jcirc;o</ref>,
      populara en sudorienta E&ubreve;ropo kaj Turkujo:
      <ekz>
        <tld/>o estas malgranda, rondforma sala bakita pasto
        <fnt>
          <aut>G&eacute;za G&aacute;rdonyi, trad. Ladislao P&aacute;sztor</aut>
          <vrk><url ref="http://verkoj.com/tradukistoj/laszlo-pasztor/la-homo-nevidebla/"
              >La homo nevidebla</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        sufi&ccirc;e ni man&gcirc;is bakitajn &scirc;afidojn,
        sufi&ccirc;e ni man&gcirc;is blankajn <tld/>ojn
        <fnt>
          <aut>M. Abesgus</aut>,
          <vrk><url
          ref="http://eo.wikisource.org/wiki/Kraljevi%C4%89_Marko">
          Kraljevi&ccirc; Marko</url></vrk>,
          <lok>1896</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bulk.0o">bulko</ref>,
      <ref cel="flan.0o">flano</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_g;&c_a;&c_ch;&c_a;</trd>
  <trd lng="de">Pogatsche</trd>
  <trd lng="fr">pogatcha</trd>
  <trd lng="pl">p&aogonek;czek</trd>
  <trd lng="ro">covrig</trd>
</drv>
</art>

