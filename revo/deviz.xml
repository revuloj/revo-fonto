<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: deviz.xml,v 1.21 2021/03/08 14:29:16 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>deviz</rad>/o
</kap>

<drv mrk="deviz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="deviz.0o.HER">
    <uzo tip="fak">HER</uzo>
    <dif>
      <ref tip="super" cel="senten.0o">Sentenco</ref> montranta
      principon
      de konduto adoptitan kiel distingilon de privata persono, firmao,
      korporacio, ofte skribitan sur insigno a&ubreve; <ref tip="malprt" cel="blazon.0o">blazono</ref>:
      <ekz>
        <ctl>libereco, egaleco, frateco</ctl> estas la <tld/>o de la
        franca Respubliko;
      </ekz>
      <ekz>
        la granda duko de Nerbia <klr>...</klr> havas sur la
        &scirc;ildo, kiel
        emblemon, asparagon kun la jena <tld/>o: <ctl>Mia sorto
        rampas</ctl>
        <fnt><bib>DonKihxoto</bib>, <lok>unua parto, &ccirc;apitro 18a,
        p.
        127a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_e;&c_v;&c_ib;&c_z;</trd>,
      <trd>&c_l;&c_jo;&c_z;&c_u;&c_n;&c_g;</trd>
    </trdgrp>
    <trd lng="bg">&c_d;&c_je;&c_v;&c_i;&c_z;</trd>
    <trdgrp lng="de">
      <trd>Devise</trd>,
      <trd>Wahlspruch</trd>,
      <trd>Sinnspruch</trd>,
      <trd>Motto</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>chant</trd>,
      <trd>slogan</trd>,
      <trd>rallying cry</trd>,
      <trd>motto</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>divisa</trd>,
      <trd>lema</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>devise<klr tip="ind"> (formule concise)</klr></trd>,
      <trd>mot d'ordre</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>jelige</trd>,
      <trd>jelmondat</trd>,
      <trd>c&iacute;merfelirat</trd>,
      <trd>szlogen</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>divisa<klr> (motto)</klr></trd>,
      <trd>impresa<klr> (motto)</klr></trd>,
      <trd>massima</trd>,
      <trd>motto</trd>
    </trdgrp>
    <trd lng="nl">devies</trd>
    <trdgrp lng="pl">
      <trd>dewiza</trd>,
      <trd>motto</trd>,
      <trd>zawo&lstroke;anie</trd>
    </trdgrp>
  <trdgrp lng="pt">
    <trd>divisa</trd>,
    <trd>lema</trd>
  </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_je;&c_v;&c_i;&c_z;</trd>,
      <trd>&c_l;&c_o;&c_z;&c_u;&c_n;&c_g;</trd>
    </trdgrp>
  </snc>
  <snc mrk="deviz.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Kambio a&ubreve; &ccirc;eko en fremda valuto, pagebla je difinita
      loko
      eksterlande kaj negocata en borso.
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_e;&c_v;&c_ib;&c_z;&c_a;</trd>,
      <trd>&c_p;&c_l;&c_a;&c_c;&c_jo;&c_zh;&c_n;&c_y;
      &c_s;&c_r;&c_o;&c_d;&c_a;&c_k; &c_u;
      &c_z;&c_a;&c_m;&c_je;&c_zh;&c_n;&c_a;&c_j;
      &c_v;&c_a;&c_l;&c_ju;&c_c;&c_je;</trd>
    </trdgrp>
    <trd lng="bg">&c_d;&c_je;&c_v;&c_i;&c_z;</trd>
    <trdgrp lng="de">
      <trd>Devisen</trd>,
      <trd>Auslandsguthaben</trd>
    </trdgrp>
    <trd lng="en">deposits at foreign banks</trd>
    <trd lng="es">divisa<klr> (monetaria)</klr></trd>
    <trd lng="fr">devise<klr> (mon&eacute;taire)</klr></trd>
    <trdgrp lng="hu">
      <trd>deviza</trd>,
      <trd>k&uuml;lf&ouml;ldi <ind>fizet&odblac;eszk&ouml;z</ind></trd>
    </trdgrp>
    <trd lng="it">divisa<klr> (monetaria)</klr></trd>
    <trd lng="nl">deviezen</trd>
    <trd lng="pl">dewiza<klr> (obca waluta)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_je;&c_v;&c_i;&c_z;&c_a;</trd>,
      <trd>&c_i;&c_s;&c_ch;&c_i;&c_s;&c_l;&c_je;&c_n;&c_i;&c_je; &c_v;
      &c_i;&c_n;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_n;&c_o;&c_j;
      &c_v;&c_a;&c_l;&c_ju;&c_t;&c_je;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>heslo slovn&#xED;</trd>,
    <trd>v&#x16F;d&#x10D;&#xED; my&#x161;lenka</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&#x5DE;&#x5B7;&#x5D8;&#x5B0;&#x5D1;&#x5BC;&#x5B5;&#x5E2;&#x5B7; &#x5D7;&#x5D5;&#x5E5;</trd>,
    <trd>&#x5E1;&#x5D9;&#x5E1;&#x5DE;&#x5D4;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>heslo</trd>,
    <trd>&#x161;ek na cudziu menu</trd>
  </trdgrp>
</drv>
</art>
