<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kortrejk.xml,v 1.10 2023/09/01 15:46:44 revo Exp $">
<kap>
  <rad>Kortrejk</rad>/o
</kap>
<drv mrk="kortrejk.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:urboj" cel="urb.0o">Urbo</ref> en la belga provinco
      <ref tip="malprt" cel="flandr.F0ujo.prov">Okcident-Flandrujo</ref>
      (3&deg;16&minute;E, 50&deg;49&minute;N):
      <ekz>
        la urbo <tld/>o posedas la plej grandan kolekton de
        Esperanto-revuoj en la mondo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_K;&c_o;&c_r;&c_t;&c_r;&c_e;&c_j;&c_k;</trd>,
    <trd>&c_K;&c_u;&c_r;&c_t;&c_r;&c_e;</trd>
  </trdgrp>
  <trd lng="de">Kortrijk</trd>
  <trd lng="fr">Courtrai</trd>
  <trd lng="hu">Kortrijk</trd>
  <trd lng="ja">&#x30B3;&#x30EB;&#x30C8;&#x30E9;&#x30A4;&#x30AF;</trd>
  <trd lng="lat">Cortoriacum</trd>
  <trd lng="nl">Kortrijk</trd>
  <trd lng="pl">Kortrijk</trd>  
  <trd lng="pt">Kortrijk</trd>
  <trd lng="ro">Kortrijk</trd>
  <trdgrp lng="ru">
    <trd>&c_K;&c_u;&c_r;&c_t;&c_r;&c_je;</trd>,
    <trd>&c_K;&c_o;&c_r;&c_t;&c_r;&c_je;&c_j;&c_k;</trd>
  </trdgrp>
</drv>
</art>
