<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>krater</rad>/o
</kap>
<drv mrk="krater.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Lar&gcirc;a vazo en kiu la antikvuloj miksis vinon kaj akvon.
    </dif>
  </snc>
  <snc>
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Malferma&jcirc;o de vulkano, tra kiu &gcirc;i el&jcirc;etas brulajn materiojn, lafon,
      ktp; vulkana elsputejo.
    </dif>
    <refgrp tip="sin">
      <ref cel="fauxk.0o">fa&ubreve;ko</ref>,
      <ref cel="abism.0o">abismo</ref>.
    </refgrp>
    <trd lng="en">crater</trd>
    <trd lng="sv">krater</trd>
  </snc>
  <trd lng="ru">&c_k;&c_r;&c_a;&c_t;&c_je;&c_r;</trd>
</drv>
</art>
