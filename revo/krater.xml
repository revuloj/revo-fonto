<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>krater</rad>/o
</kap>
<drv mrk="krater.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Lar&gcirc;a <ref tip="super" cel="vaz.0o">vazo</ref>, en kiu
      antikvaj grekoj miksis vinon kaj akvon por festenoj.
    </dif>
    <trdgrp lng="hu">
      <trd>crater</trd>,
      <trd>vegy&iacute;t&odblac; ed&eacute;ny</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Malferma&jcirc;o de <ref tip="malprt" cel="vulkan.0o">vulkano</ref>, tra
      kiu &gcirc;i el&jcirc;etas brulajn materiojn, lafon,
      ktp; vulkana elsputejo:
      <ekz>
        submara tertremo pro la eksplodo de la <tld/>o<fnt><aut>Evgeni
        Georgiev</aut>: <vrk>Vulkanoj</vrk>, <bib>Monato</bib><lok>jaro
        2001a, numero 2a, p. 22a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="abism.0o">abismo</ref>,
      <ref cel="fauxk.0o">fa&ubreve;ko</ref>.
    </refgrp>
    <trd lng="en">crater</trd>
    <trd lng="es">cr&aacute;ter</trd>
    <trd lng="hu">kr&aacute;ter</trd>
    <trd lng="sv">krater</trd>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_a;&c_t;&c_e;&c_r;</trd>
  <trd lng="de">Krater</trd>
  <trdgrp lng="fr">
    <trd>crat&egrave;re</trd>
  </trdgrp>
  <trd lng="pt">cratera</trd>
  <trd lng="ru">&c_k;&c_r;&c_a;&c_t;&c_je;&c_r;</trd>
</drv>
</art>
