<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: krater.xml,v 1.26 2023/09/01 15:46:44 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>krater</rad>/o <fnt><bib>Fab3</bib></fnt>
</kap>
<drv mrk="krater.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="krater.0o.VAZ">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Lar&gcirc;a <ref tip="super" cel="vaz.0o.VAZ">vazo</ref>, en kiu
      antikvaj grekoj miksis vinon kaj akvon por festenoj:
      <ekz>
	Aliato <klr>[&#x2026;]</klr> dedi&ccirc;is al Delfoj grandan ar&gcirc;entan
	<tld/>on
	<fnt>
	  <aut>Herodoto, trad. S. Sarafian</aut>,
	  <vrk>Historio</vrk>,
	  <lok>2006</lok>
	</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="kalik.0o.glaso">kaliko</ref>,
       <ref cel="amfor.0o">amforo</ref>
    </refgrp>
    <bld lok="&WCU;/thumb/1/1a/Crat&egrave;re_d'Eurytios_(Louvre%2C_Cp_33).jpg/320px-Crat&egrave;re_d'Eurytios_(Louvre%2C_Cp_33).jpg" prm="PD"><tld/>o de Eurytios, &ccirc;. 600 a.K., muzeo Louvre, Parizo
        <fnt>
          <aut>Tangopaso, Self-photographed</aut>
          <lok><url ref="&WCW;/File:Crat%C3%A8re_d'Eurytios_(Louvre,_Cp_33).jpg">&WCW;/File:Crat&egrave;re_d'Eurytios_(Louvre,_Cp_33).jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="en">krater</trd>
    <trdgrp lng="hu">
      <trd>crater</trd>,
      <trd>vegy&iacute;t&odblac; ed&eacute;ny</trd>
    </trdgrp>
    <trd lng="nl">amfora</trd>
    <trd lng="pl">krater <klr>(naczynie)</klr></trd>
  </snc>
  <snc mrk="krater.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Elsputejo, malferma&jcirc;o de <ref tip="malprt" cel="vulkan.0o">vulkano</ref>, tra
      kiu &gcirc;i el&jcirc;etas brulajn materiojn, lafon...:
      <ekz>
	submara tertremo pro la eksplodo de la <tld/>o
	<fnt>
	  <aut>E. Georgiev</aut>,
	  <vrk>Vulkanoj</vrk>,
	  <bib>Monato</bib>,
	  <lok>jaro 2001a, numero 2a, p. 22a</lok>
	</fnt>;
      </ekz>
      <ekz>
        okaze de la Usu-erupcio la agentejo unuafoje flugigis senpilotan aviadilon por
        foti la <tld/>on kaj kolekti gason
        <fnt><aut>HORI Jasuo</aut>: 
          <vrk><url ref="&Monato;/2000/007147.php">Erupcias maltrankvilo</url></vrk>,
          <bib>Monato</bib>
          <lok>2000/12, p. 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        oni gloras heroojn, poetojn kaj scienculojn, kiujn nia tempo ne konas sed kiuj
        estas naskotaj sur la <tld/>o de E&ubreve;ropo: en Parizo
        <fnt>
          <bib>Fab3</bib>
          <lok>Post jarmiloj</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="abism.0o">abismo</ref>,
      <ref cel="fauxk.0o">fa&ubreve;ko</ref>
    </refgrp>
    <trd lng="en">crater</trd>
    <trd lng="es">cr&aacute;ter</trd>
    <trd lng="hu">kr&aacute;ter</trd>
    <trd lng="nl">krater</trd>
    <trd lng="pl">krater</trd>
    <trd lng="sv">krater</trd>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_a;&c_t;&c_e;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>j&iacute;cen sopky</trd>,
    <trd>kr&aacute;ter</trd>
  </trdgrp>
  <trd lng="de">Krater</trd>
  <trd lng="fr">crat&egrave;re</trd>
  <trdgrp lng="ja">
    <trd>&#x706B;&#x53E3;  [<ind>&#x307B;&#x304F;&#x3061;</ind>]</trd>,
    <trd>&#x5674;&#x706B;&#x53E3;  [<ind>&#x3075;&#x3093;&#x304B;&#x3053;&#x3046;</ind>]</trd>,
    <trd>&#x30AF;&#x30EC;&#x30FC;&#x30BF;&#x30FC;</trd>,
    <trd>&#x30AF;&#x30E9;&#x30C6;&#x30EB;</trd>
  </trdgrp>
  <trd lng="pt">cratera</trd>
  <trd lng="ro">crater vulcanic</trd>
  <trd lng="ru">&c_k;&c_r;&c_a;&c_t;&c_je;&c_r;</trd>
  <trd lng="sk">kr&aacute;t&eacute;r</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_r;&c_a;&c_t;&c_je;&c_r;</trd>,
    <trd>&c_v;&c_a;&c_z;&c_a;</trd>,
    <trd>&c_zh;&c_je;&c_r;&#x43B;&#x43E;</trd>
  </trdgrp>
</drv>

<drv mrk="krater.frap0o">
  <kap>frap<tld/>o, <var><kap>frapo<tld/>o</kap></var></kap>
  <snc mrk="krater.frap0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      <ref tip="vid" cel="krater.0o.GEOL"><tld lit="K"/>o</ref>forma
      baseno, kiun ka&ubreve;zis meteor&scirc;tono falegante sur la grundon de solida
      planedo:
      <ekz>
        la teamo de la Universitato de Padovo <klr>[&#x2026;]</klr> aldonas, 
        ke la drenada skemo en la ringo estas tre
        simila al tiuj trovitaj en granda frap<tld/>oj en humidaj 
        medioj
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.eventeo.net/web/index.php/categoryblog/1858-nova-kratero-eltrovita-en-afriko">
          Nova frapkratero eltrovita en Afriko</url></vrk>,
          <lok>eventeo, 2010-03-15</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kiamaniere enbati najlon sen lasi lunajn <tld/>ojn en la muro
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2011/010337.php">Konsiloj avinaj kaj neavinaj</url></vrk>
          <lok>2011</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Einschlagkrater</trd>
  <trd lng="en">impact crater</trd>
  <trd lng="fr">crat&egrave;re d'impact</trd>
  <trd lng="pl">lej</trd>     
  <trd lng="ro">dolin&abreve;</trd>
</drv>
</art>
