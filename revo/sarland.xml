<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Sarland</rad>/o
</kap>
<drv mrk="sarland.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>Lkn</bib></fnt>
  <snc>
    <dif>
      Unu el la federaciaj &scirc;tatoj de
      <ref tip="malprt" cel="german1.G0ujo">Germanujo</ref>, situanta 
      en la sudokcidenta parto inter <ref tip="vid"
      cel="franc.F0ujo">Francujo</ref> kaj
      Rejnland-Palatinato. Regurbo kaj plej granda urbo estas Sarbruko.
    </dif>
    <trd lng="de">Saarland</trd>
    <trd lng="hu">Saar-vid&eacute;k</trd>
    <trd lng="ru">&c_S;&c_a;&c_a;&c_r;</trd>
  </snc>
</drv>

<drv mrk="sarland.0ano">
  <kap><tld lit="s"/>ano</kap>
  <snc>
    <dif>
      Civitano de Sarlando.
    </dif>
  </snc>
  <trd lng="de">Saarl&auml;nder</trd>
  <trd lng="hu">Saar-vid&eacute;ki</trd>
</drv>

<drv mrk="sarland.0a">
  <kap><tld lit="s"/>a</kap>
  <snc>
    <dif>
      Rilatanta al Sarlando.
    </dif>
  </snc>
  <trd lng="de">saarl&auml;ndisch</trd>
  <trd lng="hu">Saar-vid&eacute;ki</trd>
</drv>
</art>
