<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>metafor</rad>/o
</kap>
<drv mrk="metafo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELE</uzo>
  <uzo tip="fak">LIN</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="figur.stil0o">Stilfiguro</ref>
      per kiu oni donas al iu vorto signifon alian ol &gcirc;ia
      propra, klarigeblan per subkomprenita komparo.
    </dif>
    <refgrp tip="sin">
      <ref cel="figur.0o">figuro 4</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_t;&c_a;&c_f;&c_a;&c_r;&c_a;</trd>
  <trd lng="de">Metapher</trd>
  <trd lng="en">metaphor</trd>
  <trdgrp lng="fr">
    <trd>m&eacute;taphore</trd>,
    <trd>image<klr> (m&eacute;taphore)</klr></trd>
  </trdgrp>
  <trd lng="hu">metafora</trd>
  <trd lng="nl">beeldspraak</trd>
  <trd lng="ru">&c_m;&c_je;&c_t;&c_a;&c_f;&c_o;&c_r;&c_a;</trd>
</drv>
</art>
