<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>stadion</rad>/o, stadium/o
</kap>
<drv mrk="stadio.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      Loko destinita por atletaj a&ubreve; sportaj ekzercoj kaj konkursoj,
      kun mult&scirc;tupaj vicoj da sidlokoj por la publiko.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_d;&c_y;&c_jo;&c_n;</trd>
  <trd lng="de">Stadion</trd>
  <trdgrp lng="en">
       <trd>stadium</trd>,
       <trd>arena</trd>,
       <trd>ballpark</trd>,
       <trd>colliseum</trd>,
       <trd>grounds</trd>,
       <trd>grandstand</trd></trdgrp>
  <trd lng="fr">stade<klr tip="ind"> (de sport)</klr></trd>
  <trd lng="nl">stadion</trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_d;&c_i;&c_o;&c_n;</trd>
</drv>
</art>
