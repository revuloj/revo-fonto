<drv mrk="sxlos.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxlos.0i.seruro">
    <dif>
      Fermi per seruro:
      <ekz>
        <tld/>u vian pordon post vi<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la pordo estis nur fermita sen<tld/>e<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rigl.0i">rigli</ref>.
    </refgrp>
    <trd lng="de">abschlie&szlig;en</trd>
    <trd lng="es">cerrar con llave</trd>
    <trd lng="fr"><ind>fermer</ind> &agrave; clef</trd>
    <trdgrp lng="it">
    <trd><ind>chiudere</ind> a chiave</trd>, 
    <trd>serrare<klr> (chiudere a chiave)</klr></trd>
  </trdgrp>
  </snc>
  <snc mrk="sxlos.0i.malhelpi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malhelpi eliron de iu a&ubreve; elmontri&gcirc;on de io:
      <ekz>
        alta ordono <tld/>as mian bu&scirc;on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        pensoj <tld/>itaj en la brusto<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ili perdis la vojon kaj la dezerto ilin <tld/>is<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="it">
    <trd>chiudere<klr> (fig.)</klr></trd>, 
    <trd>serrare<klr> (chiudere - fig.)</klr></trd>
  </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_ch;&c_y;&c_n;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_y;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_z;&c_a;&c_k;&c_l;&c_ju;&c_ch;&c_v;&c_a;&c_m;</trd>
  <trd lng="cs">zamknout</trd>
  <trdgrp lng="de">
    <trd>einsperren</trd>,
    <trd>einschlie&szlig;en</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fermer</trd>,
    <trd>renfermer</trd>
  </trdgrp>
  <trd lng="en">lock</trd>
  <trd lng="he">&lamed;&nun;&ayin;&vav;&lamed;
\<ind>&nun;&ayin;&lamed;</ind>\</trd>
  <trd lng="hu">bez&aacute;r</trd>
  <trd lng="id"><baz>kunci</baz> mengunci</trd>
  <trdgrp lng="nl">
    <trd>sluiten</trd>,
    <trd>op <ind>slot doen</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fechar<klr> (com chave)</klr></trd>,
    <trd>trancar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_p;&c_i;&c_r;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_je;&c_r;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zamkn&uacute;&tcaron;</trd>
</drv>

<drv mrk="sxlos.0ilo">
  <kap><ofc>*</ofc><tld/>ilo <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5202">&scirc;los'il'</url></lok></fnt></kap>
  <snc mrk="sxlos.0ilo.seruro">
    <dif>
      Metala peco, la&ubreve;cele tran&ccirc;ita, servanta por fermi
      kaj malfermi seruron.
    </dif>
    <trd lng="pl">klucz</trd>
  </snc>
  <snc mrk="sxlos.0ilo.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tio, kio ebligas komprenon pri problemo; enkondukilo al ia
      studo:
      <ekz>
        trovi la <tld/>ilon de enigmo;
      </ekz>
      <ekz>
        tiu vortareto prezentas oportunan plenan <tld/>ilon por la
        letero<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_l;&c_ju;&c_ch;</trd>
    <trd lng="bg">&c_k;&c_l;&c_ju;&c_ch;</trd>
    <trd lng="ca">clau</trd>
    <trd lng="de">Schl&uuml;ssel</trd>
    <trd lng="en">key</trd>
    <trd lng="es">llave</trd>
    <trd lng="he">&mem;&pe;&tav;&het;</trd>
    <trd lng="hu">kulcs</trd>
    <trd lng="it">chiave</trd>
    <trd lng="nl">sleutel</trd>
    <trd lng="oc">clau</trd>
    <trd lng="pl">klucz</trd>
    <trd lng="pt">chave</trd>
    <trd lng="ru">&c_k;&c_l;&c_ju;&c_ch;</trd>
  </snc>
  <snc mrk="sxlos.0ilo.KOMP">
    <fnt>
      <bib>KompLeks</bib> 
    </fnt> 
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Simpla a&ubreve; struktura <ref tip="malprt" cel="datum.0o">datumo</ref>
      (<ref tip="sin" cel="indic.0o.KOMP">indico</ref> a&ubreve;
      <ref tip="vid" cel="rikord.0o.KOMP">rikorda</ref>
      <ref tip="super" cel="kamp.0o.dateno">kampo</ref>)
      identiganta rikordon a&ubreve; indikanta ties situon.
      studo:
      <ekz>
        &ccirc;ef<tld/>ilo;
      </ekz>
      <ekz>
        krom<tld/>ilo.
      </ekz>
    </dif>
    <ref tip="sin" cel="kon.0igilo.KOMP">konigilo</ref>
    <ref tip="vid" cel="etiked.0o.KOMP">etikedo</ref>
    <trd lng="en">key</trd>
    <trd lng="pl">klucz</trd>
  </snc>
  <trd lng="af">sleutel</trd>
  <trd lng="am">&#x1241;&#x120D;&#x134D;</trd>
  <trd lng="ar">&#x645;&#x641;&#x62A;&#x627;&#x62D;</trd>
  <trd lng="az">a&#xE7;ar</trd>
  <trd lng="bn">&#x99A;&#x9BE;&#x9AC;&#x9BF;</trd>
  <trd lng="bo">&#xF63;&#xFA1;&#xF7A;&#xF0B;&#xF58;&#xF72;&#xF42;&#xF0B;</trd>
  <trd lng="bs">klju&#x10D;</trd>
  <trd lng="co">chiavi</trd>
  <trd lng="cs">kl&iacute;&ccaron;</trd>
  <trd lng="da">n&#xF8;gle</trd>
  <trd lng="et">v&#xF5;ti</trd>
  <trd lng="eu">giltza</trd>
  <trdgrp lng="fr">
    <trd>cl&eacute;</trd>,
    <trd>clef</trd>
  </trdgrp>
  <trd lng="fy">kaai</trd>
  <trd lng="gl">clave</trd>
  <trd lng="gu">&#xA95;&#xAC0;</trd>
  <trd lng="hi">&#x915;&#x941;&#x902;&#x91C;&#x940;</trd>
  <trd lng="hr">klju&#x10D;</trd>
  <trd lng="hy">&#x570;&#x56B;&#x574;&#x576;&#x561;&#x56F;&#x561;&#x576;</trd>
  <trd lng="id">kunci</trd>
  <trd lng="is">lykill</trd>
  <trd lng="ja">&#x30AD;&#x30FC;</trd>
  <trd lng="jw">tombol</trd>
  <trd lng="ka">&#x10DB;&#x10D7;&#x10D0;&#x10D5;&#x10D0;&#x10E0;&#x10D8;</trd>
  <trd lng="km">&#x1782;&#x1793;&#x17D2;&#x179B;&#x17B9;&#x17C7;</trd>
  <trd lng="ko">&#xD0A4;</trd>
  <trd lng="lo">&#xE97;&#xEB5;&#xEC8;&#xEAA;&#xECD;&#xEB2;&#xE84;&#xEB1;&#xE99;</trd>
