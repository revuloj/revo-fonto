<drv mrk="sxlos.en0i">
  <kap>en<tld/>i, <var><kap>for<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxlos.en0i.KOMUNE">
    <dif>
      Per seruro kaj <tld/>ilo
      a&ubreve; simila rimedo <ref tip="super" cel="ferm.en0i">enfermi</ref> en iu loko:
      <ekz>
        oni forgesu nenian lokon, oni anka&ubreve; en<tld/>u ie la katojn, la terurajn
        malamikojn de miaj klientoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la pastro en<tld/>u la infektiton por sep tagoj
        <fnt><bib>MT</bib>, <lok>&Lev; 13:5</lok></fnt>;
      </ekz>
      <ekz>
        terure! ekkriis la re&gcirc;idino, oni &gcirc;in ja en<tld/>is
        <fnt>
          <bib>Fab2</bib>
          <lok>La ombro</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiuj banditoj tute ne for<tld/>os la a&jcirc;ojn, ili glate dividos ilin inter si
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        prenu la paperojn kaj en<tld/>u ilin
        <fnt>
          <bib>BdV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">uzamknout</trd>
  <trdgrp lng="de">
    <trd>einschlie&szlig;en</trd>,
    <trd>wegschlie&szlig;en</trd>
  </trdgrp>
  <trd lng="fr">enfermer<klr> (&agrave; cl&eacute;)</klr></trd>
  <trd lng="sk">uzamkn&uacute;&tcaron;</trd>
</drv>

<drv mrk="sxlos.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxlos.mal0i.KOMUNE">
    <dif>
      <ref tip="super" cel="ferm.mal0i.eniro">Malfermi</ref> per <tld/>ilo:
      <ekz>
        li prenas serurrompilojn kaj mal<tld/>as la pordon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi mal<tld/>as &scirc;rankojn
        <fnt>
          <aut>C. Piron</aut>,
          <vrk><url ref="http://esperanto.org.uk/eldonoj/piron/tekstoj/Tien.html">
          Tien</url></vrk>,
          <lok>1997</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kiam la Eternulo vidis, ke Lea estas malamata, Li mal<tld/>is &scirc;ian uteron
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 29:31</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">odemknout</trd>
  <trd lng="de">aufschlie&szlig;en</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;verrouiller</trd>,
    <trd>ouvrir<klr> (avec une cl&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="sk">odomkn&uacute;&tcaron;</trd>
</drv>
