<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fart.xml,v 1.23 2021/03/09 19:35:26 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fart</rad>/i <fnt><bib>PV</bib></fnt> 
</kap>
<drv mrk="fart.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="fart.0i.KOMUNE">
    <dif>
      Esti en bona a&ubreve; malbona stato, rilate al la funkcioj de la
      organismo:
      <ekz>
        <ind>kiel vi <tld/>as?</ind>
        <trdgrp lng="be">
          <trd>&c_ja;&c_k;
          &c_m;&c_a;&c_je;&c_c;&c_je;&c_s;&c_ja;?</trd>,
          <trd>&c_ja;&c_k;
          &c_p;&c_a;&c_ch;&c_u;&c_v;&c_a;&c_je;&c_c;&c_je;&c_s;&c_ja;?</trd>
        </trdgrp>
        <trd lng="en">how are you?</trd>
        <trd lng="fr">comment allez-vous ?</trd>
        <trd lng="hu">hogy vagy?</trd>
        <trd lng="nl">hoe <ind>gaat</ind> het?</trd>
        <trdgrp lng="pl">
          <trd>jak si&eogonek; <ind>masz</ind> ?</trd>,
          <trd>jak si&eogonek; <ind>miewasz</ind> ?</trd>,
          <trd>jak si&eogonek; <ind> czujesz</ind> ?</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_k;&c_a;&c_k;
          &c_p;&c_o;&c_zh;&c_i;&c_v;&c_a;&c_je;&c_t;&c_je;?</trd>,
          <trd>&c_k;&c_a;&c_k; &c_v;&c_a;&c_sh;&c_je;
          &c_z;&c_d;&c_o;&c_r;&c_o;&c_v;&c_mol;&c_je;?</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_je;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_ch;&c_u;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>estar</trd>,
    <trd>anar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>da&rcaron;it se</trd>,
    <trd>m&iacute;t se</trd>,
    <trd>v&eacute;st se</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>sich <ind>befinden</ind></trd>,
     <trd>sich <ind>f&uuml;hlen</ind></trd></trdgrp>
  <trd lng="en">fare</trd>
  <trd lng="es">estar de <ind>salud</ind></trd>
  <trdgrp lng="fr">
    <trd>aller<klr tip="ind"> (se porter)</klr></trd>,
    <trd>se <ind>porter</ind></trd></trdgrp>
  <trd lng="hu">&eacute;rzi mag&aacute;t<klr tip="ind"> (j&oacute;l vagy
  rosszul)</klr></trd>
  <trd lng="nl">zich <ind>voelen</ind></trd>
  <trd lng="oc">anar</trd>
  <trdgrp lng="pl">
    <trd>mie&cacute; si&eogonek;</trd>,
    <trd>miewa&cacute; si&eogonek;</trd>,
    <trd>czu&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>passar <klr tip="amb">(de sa&uacute;de)</klr></trd>,
    <trd>andar</trd>,
    <trd>estar <klr tip="amb">(bem ou mal de sa&uacute;de)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;
    &c_s;&c_je;&c_b;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">ma&tcaron; sa</trd>
</drv>

<drv mrk="fart.0o">
  <kap><ofc>*</ofc><tld/>o<fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5021">fart'o</url></lok></fnt></kap>
  <snc>
    <dif>
      Stato de ies sano:
      <ekz>
      per tro multa varto malboni&gcirc;as la farto
      </ekz>
    </dif>
    <ref tip="sin" cel="stat.san0o">sanstato</ref>
  </snc>
  <trdgrp lng="en">
    <trd>condition</trd>,
    <trd>state</trd>,
    <trd>health</trd>
  </trdgrp>
  <trd lng="fr">&eacute;tat de sant&eacute;</trd>
</drv>

<drv mrk="fart.bon0a">
  <kap>bon<tld/>a</kap>
  <snc mrk="fart.bon0a.KOMUNE">
    <dif>
      Kiu bone <tld/>as, estas
      <ref tip="vid" cel="san.0a.bonfarta">sana</ref>:
      <ekz>
        la bon<tld/>a koro de Bob Sulavi ne akcelis sian 
        ritmon
        <fnt>
          <aut>C. Piron</aut>,
          <vrk><url ref="http://www.esperanto.org.uk/eldonoj/piron/tekstoj/Cxu_li_venis_trakosme.html">
          &Ccirc;u li venis trakosme?</url></vrk>,
          <lok>1980</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">blahobytn&yacute;</trd>
  <trd lng="fr">en bonne sant&eacute;</trd>
  <trd lng="he">&#x5DE;&#x5E8;&#x5D2;&#x5D9;&#x5E9; &#x5D8;&#x5D5;&#x5D1;</trd>
  <trd lng="sk">blahobytn&yacute;</trd>
</drv>

<drv mrk="fart.bon0o">
  <kap>bon<tld/>o</kap>
  <snc mrk="fart.bon0o.KOMUNE">
    <dif>
      Stato de bona funkciado de la organismo; sano.  
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_o;&c_b;&c_r;&c_a;&c_je;
    &c_z;&c_d;&c_a;&c_r;&c_o;&c_w;&c_je;</trd>,
    <trd>&c_d;&c_o;&c_b;&c_r;&c_a;&c_je;
    &c_s;&c_a;&c_m;&c_a;&c_a;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="cs">blahobyt</trd>
  <trdgrp lng="de">
   <trd>Wohlbefinden</trd>,
   <trd>Wohlergehen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>welfare</trd>,
    <trd>well-being</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bonne <ind>sant&eacute;</ind></trd>,
    <trd>forme<klr tip="ind"> (sant&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="he">&#x5E8;&#x5D5;&#x5D5;&#x5D7;&#x5D4;</trd>
  <trd lng="hu">j&oacute; <ind>k&ouml;z&eacute;rzet</ind></trd>
  <trd lng="nl">welzijn</trd>
  <trdgrp lng="pl">
    <trd>dobre <ind>zdrowie</ind></trd>,
    <trd>dobre <ind>samopoczucie</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_h;&c_o;&c_r;&c_o;&c_sh;&c_je;&c_je;
    &c_z;&c_d;&c_o;&c_r;&c_o;&c_v;&c_mol;&c_je;</trd>,
    <trd>&c_h;&c_o;&c_r;&c_o;&c_sh;&c_je;&c_je;
    &c_s;&c_a;&c_m;&c_o;&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>blaho</trd>,
    <trd>blahobyt</trd>
  </trdgrp>
</drv>

<drv mrk="fart.bon0i">
  <kap>bon<tld/>i</kap>
  <snc mrk="fart.bon0i.KOMUNE">
    <dif>
      Bone <ref cel="fart.0i"><tld/>i</ref>,
      esti en <ref cel="fart.bon0o">bon<tld/>o</ref>.
    </dif>
  </snc>
  <trd lng="cs">m&iacute;t se dob&rcaron;e</trd>
  <trd lng="he">&#x5DC;&#x5D4;&#x5E8;&#x5D2;&#x5D9;&#x5E9; &#x5D8;&#x5D5;&#x5D1;</trd>
  <trdgrp lng="pl">
    <trd><ind>mie&cacute; si&eogonek;</ind> dobrze</trd>,
    <trd><ind>czu&cacute; si&eogonek;</ind> dobrze</trd>
  </trdgrp>
  <trd lng="sk">ma&tcaron; sa dobre</trd>
</drv>
</art>

