<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fart.xml,v 1.34 2023/11/09 19:13:16 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fart</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fart.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="fart.0i.KOMUNE">
    <dif>
      Esti en bona a&ubreve; malbona stato, rilate al la funkcioj de la
      organismo:
      <ekz>
        <ind>kiel vi <tld/>as</ind>?
        <fnt><bib>DL</bib></fnt>;
        <trdgrp lng="be">
          <trd>&c_ja;&c_k; &c_m;&c_a;&c_je;&c_c;&c_je;&c_s;&c_ja;?</trd>,
          <trd>&c_ja;&c_k; &c_p;&c_a;&c_ch;&c_u;&c_v;&c_a;&c_je;&c_c;&c_je;&c_s;&c_ja;?</trd>
        </trdgrp>
        <trd lng="de">wie geht es?</trd>
        <trd lng="en">how are you?</trd>
        <trd lng="fr">comment allez-vous ?</trd>
        <trd lng="hu">hogy vagy?</trd>
        <trd lng="nl">hoe <ind>gaat</ind> het?</trd>
        <trdgrp lng="pl">
          <trd>jak si&eogonek; <ind>masz</ind> ?</trd>,
          <trd>jak si&eogonek; <ind>miewasz</ind> ?</trd>,
          <trd>jak si&eogonek; <ind> czujesz</ind> ?</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_k;&c_a;&c_k; &c_p;&c_o;&c_zh;&c_i;&c_v;&c_a;&c_je;&c_t;&c_je;?</trd>,
          <trd>&c_k;&c_a;&c_k; &c_v;&c_a;&c_sh;&c_je; &c_z;&c_d;&c_o;&c_r;&c_o;&c_v;&c_mol;&c_je;?</trd>
        </trdgrp>
      </ekz>
      <ekz>
        kiel vi volas, amiko, respondis la soldato, <ind><tld/>u bone</ind>
        <klr>(vd <ref cel="adiaux.0">adia&ubreve;</ref>)</klr>
        <fnt>
          <bib>ElektFab</bib>
          <lok>Kamarado Gaja</lok>
        </fnt>;
        <trd lng="de">Lebe wohl</trd>
      </ekz>
      <ekz>
        &scirc;ajnas al mi, ke vi tiam falis? nu, kiel <tld/>as via nazo
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>?
      </ekz>
      <ekz>
        la scienculo <tld/>is tute ne bone, zorgoj kaj suferoj lin turmentis
        <fnt>
          <bib>Fab2</bib>
          <lok>La ombro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lavistino <tld/>is iom pli bone, la varma biero &scirc;in pli fortigis
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is bone, enviinde bone
        <fnt>
          <bib>Fab4</bib>
          <lok>Malnova pre&gcirc;eja sonorilo</lok>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_je;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_ch;&c_u;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>estar</trd>,
    <trd>anar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>da&rcaron;it se</trd>,
    <trd>m&iacute;t se</trd>,
    <trd>v&eacute;st se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sich <ind>befinden</ind></trd>,
    <trd>sich <ind>f&uuml;hlen</ind></trd>,
    <trd>gehen <klr>(Befinden)</klr></trd>
  </trdgrp>
  <trd lng="en">fare</trd>
  <trd lng="es">estar de <ind>salud</ind></trd>
  <trdgrp lng="fr">
    <trd>aller<klr tip="ind"> (se porter)</klr></trd>,
    <trd>se <ind>porter</ind></trd></trdgrp>
  <trd lng="hu">&eacute;rzi mag&aacute;t<klr tip="ind"> (j&oacute;l vagy rosszul)</klr></trd>
  <trd lng="ja">&#xFF5E;<ind>&#x306E;&#x5065;&#x5EB7;&#x72B6;&#x614B;&#x3067;&#x3042;&#x308B; 
  </ind><pr>&#x306E;&#x3051;&#x3093;&#x3053;&#x3046;&#x3058;&#x3087;&#x3046;&#x305F;&#x3044;&#x3067;&#x3042;&#x308B;</pr></trd>
  <trd lng="nl">zich <ind>voelen</ind></trd>
  <trd lng="oc">anar</trd>
  <trdgrp lng="pl">
    <trd>mie&cacute; si&eogonek;</trd>,
    <trd>miewa&cacute; si&eogonek;</trd>,
    <trd>czu&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>passar <klr tip="amb">(de sa&uacute;de)</klr></trd>,
    <trd>andar</trd>,
    <trd>estar <klr tip="amb">(bem ou mal de sa&uacute;de)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol; &c_s;&c_je;&c_b;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">ma&tcaron; sa</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&#x43F;&#x43E;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i; &c_s;&c_je;&c_b;&c_je;</trd>,
    <trd>&#x43F;&#x43E;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="fart.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Ies <ref tip="sin" cel="stat.san0o">sanstato</ref>:
      <ekz>
        per tro multa varto malboni&gcirc;as la <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        li demandis ilin pri ilia <tld/>o, kaj diris: &ccirc;u bone <tld/>as via maljuna patro
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 43:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili demandis sin reciproke pri la <tld/>o kaj eniris en la tendon
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 18:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de la patro kaj la estonteco de la infanoj maltrankviligas min
        <fnt><bib>IK</bib></fnt>;
      </ekz>
      <ekz>
        vi kredeble neniam pensis, ke vi vidos min iam en tia bona <tld/>o!
        <fnt><bib>DL</bib></fnt>;
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Befinden</trd>
  <trdgrp lng="en">
    <trd>condition</trd>,
    <trd>state</trd>,
    <trd>health</trd>
  </trdgrp>
  <trd lng="fr">&eacute;tat de sant&eacute;</trd>
  <trd lng="ja">&#x5065;&#x5EB7;&#x72B6;&#x614B;  <pr>&#x3051;&#x3093;&#x3053;&#x3046;&#x3058;&#x3087;&#x3046;&#x305F;&#x3044;</pr></trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_s;&c_t;&c_o;&c_ja;&c_n;&c_i;&c_je; &c_z;&c_d;&c_o;&c_r;&c_o;&c_v;&c_mol;&c_ja;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_o;&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_a;&#x43C;&#x43E;&#x43F;&#x43E;&c_ch;&c_u;&c_t;&c_t;&#x44F;</trd>,
    <trd>&c_s;&c_t;&c_a;&#x43D; &c_z;&c_d;&#x43E;&c_r;&#x43E;&c_v;&#x2019;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="fart.bon0a">
  <kap>bon<tld/>a</kap>
  <snc mrk="fart.bon0a.KOMUNE">
    <dif>
      Kiu bone <tld/>as, estas
      <ref tip="vid" cel="san.0a.bonfarta">sana</ref>:
      <ekz>
        mi <klr>[estis]</klr> bon<tld/>a en mia palaco
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 4:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i balda&ubreve; pretigos por ili teon, por ke ili denove fari&gcirc;u bon<tld/>aj kaj fre&scirc;aj
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bon<tld/>a koro de <nom>Bob Sulavi</nom> ne akcelis sian
        ritmon
        <fnt>
          <aut>C. Piron</aut>,
          <vrk><url ref="http://www.esperanto.org.uk/eldonoj/piron/tekstoj/Cxu_li_venis_trakosme.html">
          &Ccirc;u li venis trakosme?</url></vrk>,
          <lok>1980</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bo">&#xF42;&#xFB2;&#xF72;&#xF42;&#xF0B;</trd>
  <trd lng="cs">blahobytn&yacute;</trd>
  <trd lng="de">wohlauf</trd>
  <trd lng="fr">en bonne sant&eacute;</trd>
  <trd lng="he">&mem;&resh;&gimel;&yod;&shin; &tet;&vav;&bet;</trd>
  <trdgrp lng="ja">
    <trd>&#x5065;&#x5EB7;&#x306A;  <pr>&#x3051;&#x3093;&#x3053;&#x3046;&#x306A;</pr></trd>,
    <trd>&#x305F;&#x3063;&#x3057;&#x3083;&#x306A;</trd>
  </trdgrp>
  <trd lng="sk">blahobytn&yacute;</trd>
</drv>

<drv mrk="fart.bon0o">
  <kap>bon<tld/>o</kap>
  <snc mrk="fart.bon0o.KOMUNE">
    <dif>
      Stato de bona funkciado de la organismo; <ref tip="sin" cel="san.0o">sano</ref>:
      <ekz>
        la re&gcirc;o Dario skribis al &ccirc;iuj popoloj <klr>[&#x2026;]</klr>: kresku via bon<tld/>o!
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 6:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia sankteco ne montris bon<tld/>on
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun la kreskado de la arbo kreskis anka&ubreve; &gcirc;ia bon<tld/>o, &gcirc;ia neesprimeble
        feli&ccirc;iga sopirado atingi &ccirc;iam pli altajn celojn
        <fnt>
          <bib>Fab4</bib>
          <lok>Lasta son&gcirc;o de maljuna kverko</lok>
        </fnt>;
      </ekz>
      <ekz>
        sin gardi devas tiu, / De kies sankta vivo ja dependas / La vivo kaj bon<tld/>o
        de tre multaj, / Se re&gcirc;o mortas, li ne mortas sola
        <fnt>
          <bib>Hamlet</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_o;&c_b;&c_r;&c_a;&c_je; &c_z;&c_d;&c_a;&c_r;&c_o;&c_w;&c_je;</trd>,
    <trd>&c_d;&c_o;&c_b;&c_r;&c_a;&c_je; &c_s;&c_a;&c_m;&c_a;&c_a;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="cs">blahobyt</trd>
  <trdgrp lng="de">
   <trd>Wohlbefinden</trd>,
   <trd>Wohlergehen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>welfare</trd>,
    <trd>well-being</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bonne <ind>sant&eacute;</ind></trd>,
    <trd>forme<klr tip="ind"> (sant&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="he">&resh;&vav;&vav;&het;&he;</trd>
  <trd lng="hu">j&oacute; <ind>k&ouml;z&eacute;rzet</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x5065;&#x5EB7;&#x72B6;&#x614B;  <pr>&#x3051;&#x3093;&#x3053;&#x3046;&#x3058;&#x3087;&#x3046;&#x305F;&#x3044;</pr></trd>,
    <trd>&#x798F;&#x7949;  <pr>&#x3075;&#x304F;&#x3057;</pr></trd>,
    <trd>&#x539A;&#x751F;  <pr>&#x3053;&#x3046;&#x305B;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">welzijn</trd>
  <trdgrp lng="pl">
    <trd>dobre <ind>zdrowie</ind></trd>,
    <trd>dobre <ind>samopoczucie</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_h;&c_o;&c_r;&c_o;&c_sh;&c_je;&c_je; &c_z;&c_d;&c_o;&c_r;&c_o;&c_v;&c_mol;&c_je;</trd>,
    <trd>&c_h;&c_o;&c_r;&c_o;&c_sh;&c_je;&c_je; &c_s;&c_a;&c_m;&c_o;&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>blaho</trd>,
    <trd>blahobyt</trd>
  </trdgrp>
  <trd lng="uk">&c_g;&c_a;&c_r;&#x43D;&c_je; &c_s;&c_a;&#x43C;&#x43E;&#x43F;&#x43E;&c_ch;&c_u;&c_t;&c_t;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x5B89;&#x5EB7; <pr>&#x101;nk&#x101;ng</pr></trd>,
    <trd>&#x4FDD;&#x6301;&#x826F;&#x597D;&#x72B6;&#x6001; <pr>b&#x1CE;och&#xED;li&#xE1;ngh&#x1CE;ozhu&#xE0;ngt&#xE0;i</pr></trd>,
    <trd>&#x7994; <pr>t&#xED;</pr></trd>,
    <trd>&#x6103; <pr>xu&#x101;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fart.bon0i">
  <kap>bon<tld/>i</kap>
  <snc mrk="fart.bon0i.KOMUNE">
    <dif>
      Bone <ref cel="fart.0i"><tld/>i</ref>,
      esti en <ref cel="fart.bon0o">bon<tld/>o</ref>:
      <ekz>
        mi ne min sentas tre bon<tld/>anta
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;is la <ref cel="vid.re0o">revido</ref>, do, bon<tld/>u
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni povas <klr>[&#x2026;]</klr> bon<tld/>i, malkovrante vere grandiozajn pejza&gcirc;ojn!
        <fnt>
          <bib>Monato</bib>
          <aut>Marc Gassion</aut>
          <vrk><url ref="&Monato;/2003/007934.html">Aude</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        suferi por mi signifus bon<tld/>i anta&ubreve; Dio
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2003/007949.html">Sur propra tereno propra
          pre&gcirc;ejo</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        nun granda parto de la e&ubreve;ropanoj bon<tld/>as
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2005/008594.php">Transnacie bonfari</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        da&ubreve;re bon<tld/>as kapitalistoj, kies profitoj kreskas spite al a&ubreve; parte e&ccirc; pro
        <klr>[&#x2026;]</klr> konfliktoj kaj problemoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2007/009366.php">Problemoj ne solvataj</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">m&iacute;t se dob&rcaron;e</trd>
  <trd lng="de">gut <ind>gehen</ind></trd>
  <trd lng="he">&lamed;&he;&resh;&gimel;&yod;&shin; &tet;&vav;&bet;</trd>
  <trdgrp lng="pl">
    <trd><ind>mie&cacute; si&eogonek;</ind> dobrze</trd>,
    <trd><ind>czu&cacute; si&eogonek;</ind> dobrze</trd>
  </trdgrp>
  <trd lng="ru">&c_h;&c_o;&c_r;&c_o;&c_sh;&c_o; &c_s;&c_je;&c_b;&c_ja; &c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">ma&tcaron; sa dobre</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&#x43E;&c_b;&c_r;&c_je; &#x43C;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&c_d;&#x43E;&c_b;&c_r;&c_je; &c_s;&c_je;&c_b;&c_je;&#xA0;&#x43F;&#x43E;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43C;&c_a;&c_t;&c_i; &c_g;&c_a;&c_r;&#x43D;&c_je; &c_s;&c_a;&#x43C;&#x43E;&#x43F;&#x43E;&c_ch;&c_u;&c_t;&c_t;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="fart.mis0o">
  <kap>mis<tld/>o,
    <var><kap>malbon<tld/>o</kap></var>
  </kap>
  <snc mrk="fart.mis0o.KOMUNE">
    <dif>
      Malbona, malagrabla <tld/>o:
      <ekz>
        hor&scirc;an&gcirc;a mis<tld/>o
        <fnt>
          <bib>Monato</bib> <aut>Roberto PIGRO</aut>:
          <vrk>Dormu bone, vivu bone (per melatonino)</vrk>.
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">indisposition</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_o;&c_h;&c_o;&c_je; &c_s;&c_a;&c_m;&c_o;&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>,
    <trd>&c_n;&c_je;&c_d;&c_o;&c_m;&c_o;&c_g;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

</art>

