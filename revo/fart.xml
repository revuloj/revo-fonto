<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fart</rad>/i
</kap>
<drv mrk="fart.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti en bona a&ubreve; malbona stato, rilate al la funkcioj de 
      l' organismo:
      <ekz>
        <ind>kiel vi <tld/>as?</ind>
        <trd lng="fr">comment allez-vous?</trd>
        <trdgrp lng="ru">
          <trd>&c_k;&c_a;&c_k; &c_p;&c_o;&c_zh;&c_i;&c_v;&c_a;&c_je;&c_t;&c_je;?</trd>,
          <trd>&c_k;&c_a;&c_k; &c_v;&c_a;&c_sh;&c_je; &c_z;&c_d;&c_o;&c_r;&c_o;&c_v;&c_mol;&c_je;?</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">m&iacute;t se</trd>
  <trdgrp lng="de">
     <trd>sich <ind>befinden</ind></trd>,
     <trd>sich <ind>f&uuml;hlen</ind></trd></trdgrp>
  <trd lng="en">fare</trd>
  <trdgrp lng="fr">
    <trd>aller<klr tip="ind"> (se porter)</klr></trd>,
    <trd>se <ind>porter</ind></trd></trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol; &c_s;&c_je;&c_b;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="fart.bon0o">
  <kap>bon<tld/>o</kap>
  <snc>
    <dif>
      Stato de bona funkciado de l' organismo; sano.  
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Wohlbefinden</trd>,
     <trd>Wohlergehen</trd></trdgrp>
  <trdgrp lng="fr">
    <trd>bonne <ind>sant&eacute;</ind></trd>,
    <trd>forme<klr tip="ind"> (sant&eacute;)</klr></trd></trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_h;&c_o;&c_r;&c_o;&c_sh;&c_je;&c_je; &c_z;&c_d;&c_o;&c_r;&c_o;&c_v;&c_mol;&c_je;</trd>,
    <trd>&c_h;&c_o;&c_r;&c_o;&c_sh;&c_je;&c_je; &c_s;&c_a;&c_m;&c_o;&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>

