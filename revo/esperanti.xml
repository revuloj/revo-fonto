<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: esperanti.xml,v 1.8 2019/10/04 06:10:18 revo Exp $">
<kap>
  <rad>Esperanti</rad>/o
</kap>
<drv mrk="esperanti.0o">
  <kap><tld/>o</kap>
  <snc mrk="esperanti.0o.ESP">
    <uzo tip="fak">ESP</uzo>
    <dif>
      <ref tip="dif" cel="espera.0ujo">Esperantujo</ref>:
      <ekz>
        ni celas kulturon de paco anka&ubreve; en <tld/>o
        <fnt>
          <bib>Monato</bib>
          <lok><url ref="http://steloj.de/esperanto/monato2/007019.html"
              >http://steloj.de/esperanto/monato2/007019.html</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        priliteraturaj esploroj en <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="https://www.monato.be/2013/010956.php"
              >Porkonkursa reflekto</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        en <tld/>o la anta&ubreve;enemuloj ne povas resti pli longe sen
        domo
        <fnt><bib>Lanti</bib>,
          <lok>&leftquot;Liberiga Stelo&rightquot; al la Verdru&gcirc;uloj,
          Provizoreco de la Statutoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">sv&ecaron;t <klr>(hnut&iacute;)</klr> esperanta</trd>
  <trdgrp lng="de">
    <trd>Esperantowelt</trd>,
    <trd>Esperantien</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Esp&eacute;rantie</trd>,
    <trd>pays de l'esp&eacute;ranto</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>Eszperancia</trd>,
    <trd>Eszperant&oacute;orsz&aacute;g</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>Esperantia</trd>,
    <trd>comunit&agrave; <ind>esperantista</ind></trd>
  </trdgrp>
  <trd lng="pl">Esperancja</trd>
  <trdgrp lng="pt">
    <trd>Esper&acirc;ntia</trd>,
    <trd>pa&iacute;s do <ind>Esperanto</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_E;&c_s;&c_p;&c_je;&c_r;&c_a;&c_n;&c_t;&c_i;&c_d;&c_a;</trd>
  <trd lng="sk">svet esperanta</trd>
</drv>
</art>

