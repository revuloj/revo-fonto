<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Esperanti</rad>/o
</kap>
<drv mrk="esperanti.0o">
  <kap><tld/>o</kap>
  <snc mrk="Esperanti.0o.ESP">
    <uzo tip="fak">ESP</uzo>
    <ref tip="dif" cel="espera.0ujo">Esperantujo</ref>
    <ekz>
      en <tld/>o la anta&ubreve;enemuloj ne povas resti pli longe sen domo
      <fnt><bib>Lanti</bib>, <lok>&leftquot;Liberiga Stelo&rightquot; al la Verdru&gcirc;uloj,
      Provizoreco de la Statutoj</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="de">Esperantowelt</trd>
  <trdgrp lng="fr">
    <trd>Esp&eacute;rantie</trd>,
    <trd>pays de l'esp&eacute;ranto</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>Eszperancia</trd>,
    <trd>Eszperant&oacute;orsz&aacute;g</trd>
  </trdgrp>
  <trd lng="ru">&c_E;&c_s;&c_p;&c_je;&c_r;&c_a;&c_n;&c_t;&c_i;&c_d;&c_a;</trd>
</drv>
</art>

