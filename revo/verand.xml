<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: verand.xml,v 1.27 2024/05/19 11:18:31 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>verand</rad>/o
</kap>

<drv mrk="verand.0o">
  <kap><tld/>o</kap>
  <snc mrk="verand.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Tegmenthava kaj vitrita <ref cel="teras.0o">teraso</ref> a&ubreve; <ref cel="balkon.0o">balkono</ref>:
      <ekz>
        Manjo <klr>[&#x2026;]</klr> eliris sur la postan <tld/>on
        <fnt><bib>Ne&gcirc;aBlovado</bib></fnt>;
      </ekz>
      <ekz>
        plantoj kreskis dense sur la lar&gcirc;a anta&ubreve;a <tld/>o
        <fnt><bib>DOz</bib>, <lok>&leftquot;la valo de vo&ccirc;oj&rightquot;</lok></fnt>.
      </ekz>
      <ekz>
        subite, kvaza&ubreve; el&ccirc;eni&gcirc;inte, ekdancis amba&ubreve; salonoj kaj
         post ili ekdancis la <tld/>o
         <fnt><bib>MkM</bib>, <lok/></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="galeri.0o.ekstera">galerio<sncref/></ref>,
      <ref cel="porcx.0o">por&ccirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_e;&c_r;&c_a;&c_n;&c_d;&c_a;</trd>
  <trd lng="bg">&c_v;&c_je;&c_r;&c_a;&c_n;&c_d;&c_a;</trd>
  <trd lng="ca">veranda</trd>
  <trd lng="cs">veranda <klr>(zasklen&aacute; nebo otev&rcaron;en&aacute;)</klr></trd>
  <trdgrp lng="de">
    <trd>Veranda</trd>
    <trd>Wintergarten</trd>
  </trdgrp>
  <trd lng="el">&my;&pi;&alfa;&lambda;&kappa;&omikron_ton;&ny;&jota;</trd>
  <trd lng="en">veranda</trd>
  <trd lng="es">veranda</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&f_y;&a_w;&a_A;&a_n;</trd>,
    <trd>&a_m;&a_h;&a_t;&a_A;&a_b;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">veranda</trd>
  <trd lng="hu">veranda</trd>
  <trdgrp lng="id">
    <trd>beranda</trd>,
    <trd>balkon</trd>,
    <trd>teras</trd>,
    <trd>langkan</trd>,
    <trd>serambi</trd>,
    <trd>emper<klr>(an)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30D9;&#x30E9;&#x30F3;&#x30C0;  <pr>&#x3079;&#x3089;&#x3093;&#x3060;</pr></trd>,
    <trd>&#x7E01;&#x5074;  <pr>&#x3048;&#x3093;&#x304C;&#x308F;</pr></trd>
  </trdgrp>
  <trd lng="nl">veranda</trd>
  <trd lng="pl">weranda</trd>
  <trd lng="pt">varanda</trd>
  <trd lng="ru">&c_v;&c_je;&c_r;&c_a;&c_n;&c_d;&c_a;</trd>
  <trd lng="sk">veranda</trd>
  <trd lng="uk">&c_v;&c_je;&c_r;&c_a;&#x43D;&c_d;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x5ECA; <pr>l&#xE1;ng</pr></trd>,
    <trd>&#x8D70;&#x5ECA; <pr>z&#x1D2;ul&#xE1;ng</pr></trd>,
    <trd>&#x5E91; <pr>w&#x1D4;</pr></trd>,
    <trd>&#x5EE1; <pr>w&#x1D4;</pr></trd>
  </trdgrp>
</drv>
</art>
