<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: arkeol.xml,v 1.25 2021/02/28 18:35:45 revo Exp $">
<kap>
  <ofc>3</ofc><rad>arkeolog</rad>/o,
  <var><kap><ofc>3</ofc><rad var="&hcirc;">ar&hcirc;eolog</rad>/o</kap></var>
</kap>

<drv mrk="arkeol.0o">
  <kap><tld/>o, <var><kap><tld var="&hcirc;"/>o</kap></var></kap>
  <snc mrk="arkeol.0o.arkeologiisto">
    <dif>
      <ref tip="dif" cel="arkeol1.0isto">Arkeologiisto</ref>:
      <ekz>
        la <tld var="&hcirc;"/>oj promesas al si ri&ccirc;an rikolton, kiam la in&gcirc;enieroj
        finos la laborojn senakvigajn
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj,
        bagateloj</lok></fnt>.
      </ekz>
      <ekz>
        rompopecoj elfositaj de <tld/>oj
        <fnt><aut>Bernard Golden</aut>:
          <vrk>&Ccirc;ina ceramika historio (Zhao Hongsheng, &Ccirc;ina ceramiko)</vrk>,
          <bib>Monato</bib><lok>jaro 1995a, numero 8a, p. 22a</lok>
        </fnt>;
      </ekz>
      <ekz>
        juna amatora <tld/>o trovis la servicon &ccirc;e Balatono sed ne taksis &gcirc;in valora
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007077.html">Servico de Sevso</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj kaj historiistoj priskribas al la publiko, kiel la romanoj vivis kaj
        militis
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="https://www.monato.be/2004/008467.html">Kampoj de Soria Antonio Machado, universala poeto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_h;&c_je;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="bg">&c_a;&c_r;&c_h;&c_je;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="ca">arque&ograve;leg</trd>
  <trd lng="es">arque&oacute;logo</trd>
  <trd lng="de">Arch&auml;ologe</trd>
  <trd lng="fr">arch&eacute;ologue</trd>
  <trd lng="he">&#x5D0;&#x5B7;&#x5E8;&#x5DB;&#x5B5;&#x5D9;&#x5D0;&#x5D5;&#x5B9;&#x5DC;&#x5D5;&#x5B9;&#x5D2;</trd>
  <trdgrp lng="hu">
    <trd>r&eacute;g&eacute;sz</trd>,
    <trd>archeol&oacute;gus</trd>
  </trdgrp>
  <trd lng="it">archeologo</trd>
  <trdgrp lng="nl">
    <trd>archeoloog</trd>,
    <trd>oudheidkundige</trd>
  </trdgrp>
  <trd lng="pl">archeolog</trd>
  <trd lng="ro">arheolog</trd>
  <trd lng="ru">&c_a;&c_r;&c_h;&c_je;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="sk">archeol&oacute;g</trd>
  <trd lng="sv">arkeolog</trd>
  <trd lng="sk">archeoll&oacute;g</trd>
  <trd lng="cs">archeollog</trd>
</drv>
</art>
