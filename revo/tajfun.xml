<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap><rad>tajfun</rad>/o</kap>

    <drv mrk="tajfun.0o">
      <kap><tld/>o</kap>
      <snc>
	<uzo tip="fak">MET</uzo>
	<dif>
          Tropika <ref tip="super" cel="ciklon.0o">ciklono</ref>,
          kirloventego esti&gcirc;anta super oceano:
          <ekz>
            Monstraj manoj kirlas la vivmaron,
            Preparas abismojn sub la ondokrest';
            Fora fulm' avertas la homaron
            Pri nova <tld/>o, pri sanga tempest'
            <fnt>
              <aut>Julio BAGHY</aut>:
              <vrk>Kavaliroj de l' Tutmonda Paco</vrk>.
              <lok>N-ro 61 en &leftquot;La vagabondo kantas&rightquot; (1937)</lok>
            </fnt>.
          </ekz>
	</dif>
      </snc>
      <trd lng="en">typhoon</trd>
      <trdgrp lng="ru">
        <trd>&c_t;&c_a;&c_j;&c_f;&c_u;&c_n;</trd>,
        <trd>&c_t;&c_r;&c_o;&c_p;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; &c_c;&c_i;&c_k;&c_l;&c_o;&c_n;</trd>
      </trdgrp>
    </drv>

  </art>

<!--
$Log$
-->
</vortaro>