<drv mrk="uz.0o">
  <kap><tld/>o</kap>
  <snc mrk="uz.0o.KOMUNE">
    <dif>
      Preno de io kiel rimedo:
      <ekz>
        la elfknabino jam dancis <klr>[&#8230;]</klr> &ccirc;arme farante <tld/>on de siaj &scirc;aloj
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la brikoj <klr>[&#8230;]</klr> rompi&gcirc;is, sed ili anka&ubreve; trovis <klr>[&#8230;]</klr> <tld/>on
        <fnt>
          <bib>Fab3</bib>
          <lok>Io</lok>
        </fnt>;
      </ekz>
      <ekz>
        fari <tld/>on el &ccirc;iu parteto
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
         post la <tld/>o de la aerostato mi &gcirc;in forbruligos
         <fnt><bib>Fab3</bib>, <lok>&scirc;tono de la sa&gcirc;uloj</lok></fnt>;
      </ekz>
      <ekz>
        oni bezonis jam helpi al si per priskriba <tld/>o de kelke da vortoj
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        li faris <tld/>on de la rajto, kiun oni donis al li
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi jam per tiu ruz-<tld/>o atingis la celon
        <fnt>
          <bib>LSB</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="util.0o">utilo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
     <trd>&c_k;&c_a;&c_r;&c_y;&c_s;&c_t;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
     <trd>&c_u;&c_zh;&c_y;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pou&zcaron;&iacute;v&aacute;n&iacute;</trd>,
    <trd>po&zcaron;&iacute;v&aacute;n&iacute;</trd>,
    <trd>upot&rcaron;eben&iacute;</trd>,
    <trd>u&zcaron;&iacute;v&aacute;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gebrauch</trd>,
    <trd>Anwendung</trd>
  </trdgrp><trd lng="es">uso</trd>
  <trd lng="fi">k&auml;ytt&ouml;</trd>
  <trd lng="fr">usage</trd>
  <trdgrp lng="ru">
     <trd>&c_u;&c_p;&c_o;&c_t;&c_r;&c_je;&c_b;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
     <trd>&c_i;&c_s;&c_p;&c_o;&c_l;&c_mol;&c_z;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
     <trd>&c_p;&c_r;&c_i;&c_m;&c_je;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pou&zcaron;&iacute;vanie</trd>,
    <trd>upotrebenie</trd>
  </trdgrp>
  <trd lng="th">&#3585;&#3634;&#3619;&#3651;&#3594;&#3657;</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&c_zh;&c_i;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_u;&c_zh;&c_i;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_i;&c_s;&c_t;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_v;&c_i;&c_k;&c_o;&c_r;&c_i;&c_s;&c_t;&c_a;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="uz.0ado">
   <kap><ofc>*</ofc><tld/>ado</kap>
   <snc mrk="uz.0ado.KOMUNE">
     <dif>
       Da&ubreve;ra, ofta <tld/>o:
      <ekz>
        li ordonis instrui al la Judoj la <tld/>adon de pafarko
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 1:18</lok>
        </fnt>;
      </ekz>
       <ekz>
         &gcirc;ia <tld/>ado <klr>[de la artikolo]</klr> estas tia sama
         kiel en la aliaj lingvoj
         <fnt><bib>F</bib>, <lok>&FE; 27</lok></fnt>;
       </ekz>
      <ekz>
        solvi la demandon, kia nome arta lingvo estos enkondukita en komunan <tld/>adon
        <fnt>
          <bib>EE</bib>
          <lok>VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        la praktika <tld/>ado de nia lingvo fari&gcirc;as &ccirc;iam pli kaj pli granda
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en Dresden en la 17a de a&ubreve;gusto 1908</vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ambro, okupita de fra&ubreve;lino <klr>[&#8230;]</klr> por &scirc;ia propra <tld/>ado
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        junuloj tenis sub la brako grandajn librojn <klr>[&#8230;]</klr> duone dis&scirc;iritajn de
        uzado
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
   <trdgrp lng="be">
     <trd>&c_k;&c_a;&c_r;&c_y;&c_s;&c_t;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
     <trd>&c_u;&c_zh;&c_y;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>pou&zcaron;&iacute;v&aacute;n&iacute;</trd>,
    <trd>po&zcaron;&iacute;v&aacute;n&iacute;</trd>,
    <trd>upot&rcaron;eben&iacute;</trd>,
    <trd>u&zcaron;&iacute;v&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Gebrauch</trd>
  
  <trd lng="es">utilizaci&oacute;n</trd>
  <trd lng="fi">k&auml;ytt&ouml;</trd>
  <trd lng="fr">utilisation</trd>
  <trd lng="hu">haszn&aacute;lat</trd>
  <trd lng="nl">gebruik</trd>
  <trd lng="pt">utiliza&ccedil;&atilde;o</trd>
  <trdgrp lng="ru">
     <trd>&c_u;&c_p;&c_o;&c_t;&c_r;&c_je;&c_b;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
     <trd>&c_i;&c_s;&c_p;&c_o;&c_l;&c_mol;&c_z;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>pou&zcaron;&iacute;vanie</trd>,
    <trd>upotrebenie</trd>
  </trdgrp>
  <trd lng="sv">anv&auml;ndning</trd>
  <trd lng="th">&#3585;&#3634;&#3619;&#3651;&#3594;&#3657;</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&c_zh;&c_i;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_u;&c_zh;&c_i;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_i;&c_s;&c_t;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_v;&c_i;&c_k;&c_o;&c_r;&c_i;&c_s;&c_t;&c_a;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="uz.0ajxo">
  <kap><ofc>*</ofc><tld/>a&jcirc;o</kap>
  <snc mrk="uz.0ajxo.KOMUNE">
    <dif>
      &Ccirc;io, kion oni <tld/>as:
      <ekz>
        amba&ubreve; <tld/>a&jcirc;oj sekve de tro multa frotado estis proksima al forvi&scirc;i&gcirc;o
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
