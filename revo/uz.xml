<drv mrk="uz.tro0i">
  <kap><ofc>*</ofc>tro<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="uz.tro0i.KOMUNE">
    <dif>
      <tld lit="U"/>i super la &gcirc;usta, ordinara mezuro:
      <ekz>
        unu vorton ankora&ubreve;, &scirc;i diris, se mi ne tro<tld/>as vian komplezon
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni ekstreme tro<tld/>is &ccirc;i tiun komparon, kiu nun rememorigas ion tute alian
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi timus tro<tld/>i vian tiel grandvaloran tempon
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        li estis dorlotata, tro<tld/>is sian sukceson, faris diversajn freneza&jcirc;ojn
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia bona edukiteco detenis min tro<tld/>i ilian regalemon
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        fidante je la malfacila kontrolebleco de siaj rakontoj <klr>[ili]</klr> ofte
        tro<tld/>as la kredemon de la legantaro
        <fnt>
          <bib>VaK</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="disip.0i">disipi</ref>,
<ref cel="sxpar.mal0i">mal&scirc;pari</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
     <trd>&c_z;&c_l;&c_o;&c_w;&c_zh;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_u;&c_zh;&c_y;&c_v;&c_a;&c_c;&c_mol; &c_p;&c_r;&c_a;&c_z;&c_mol;&c_m;&c_je;&c_r;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nadu&zcaron;&iacute;t</trd>,
    <trd>nadu&zcaron;&iacute;vat</trd>,
    <trd>zneu&zcaron;&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="de">
