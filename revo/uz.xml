<drv mrk="uz.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="uz.for0i.KOMUNE">
    <dif>
      <ref tip="super" cel="konsum.0i">Konsumi</ref> per <tld/>ado:
      <ekz>
        &ccirc;ar la sumo estis pli granda, ol li iam anta&ubreve;e for<tld/>is pojare, li provis
        trovi alian manieron forfluigi la monon
        <fnt>
          <bib>HsT</bib>
        </fnt>;
      </ekz>
      <ekz>
        for<tld/>i sian tempon de la neceso al la superfluo
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi for<tld/>as vian energion de la kreanta laboro
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni for<tld/>is &ccirc;iun farbon, <klr>[&#8230;]</klr> &ccirc;u bruna, verda, viola, vermiljona,
        flava a&ubreve; oran&gcirc;a
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 19</lok>
        </fnt>;
      </ekz>
      <ekz>
        presiloj havas nur normajn paperformatojn, do anka&ubreve; deklinia notico for<tld/>as
        folion A4
        <fnt>
          <bib>Monato</bib>
          <aut>S. Maul</aut>
          <lok><url ref="&Steloj;monato2/007176.html">&Steloj;monato2/007176.html</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        pli rapida procesoro for<tld/>as pli da elektro
        <fnt>
          <bib>Monato</bib>
          <aut>Zoran &Cacute;IRI&Cacute;</aut>
          <vrk><url ref="&Monato;/2003/008165.html">A&ccirc;eti tekokomputilon</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        en Strasburgo oni for<tld/>as sian tempon ne nur por muziko
        <fnt>
          <bib>LOdE</bib>
          <lok>2003,  &#8470; 4 (102)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
   <trdgrp lng="be">
     <trd>&c_s;&c_p;&c_a;&c_zh;&c_y;&c_c;&c_mol;</trd>,
     <trd>&c_v;&c_y;&c_k;&c_a;&c_r;&c_y;&c_s;&c_t;&c_a;&c_c;&c_mol;</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>spot&rcaron;ebovat se u&zcaron;&iacute;v&aacute;n&iacute;m</trd>,
    <trd>zcela se opot&rcaron;ebit</trd>
  </trdgrp>
  <trd lng="de">aufbrauchen</trd>
  <trdgrp lng="es">
    <trd>terminar</trd>,
    <trd>agotar</trd>,
  </trdgrp>
  <trdgrp lng="fi">
    <trd>k&auml;ytt&auml;&auml; loppuun</trd>
  </trdgrp>
  <trd lng="fr">&eacute;puiser</trd>
  <trdgrp lng="hu">
