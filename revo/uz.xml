<drv mrk="uz.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="uz.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="pren.0i.mane">Preni</ref> ion kiel
      rimedon por atingi celon:
      <ekz>
         mi devis <tld/>i mian po&scirc;an tran&ccirc;ilon
         <fnt><bib>F</bib>, <lok>&FE; 34</lok></fnt>;
      </ekz>
      <ekz>
        viajn plej bonajn junulojn kaj viajn azenojn li <klr>[&#8230;]</klr> <tld/>os por siaj
        laboroj
        <fnt>
          <bib>MT</bib>
          <lok>&Sa1; 8:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        se vi e&ccirc; <klr>[&#8230;]</klr> <tld/>us por vi multe da sapo, via malvirto tamen restos
        <fnt>
          <bib>MT</bib>
          <lok>&Jer; 2:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni <tld/>u do ruzon
        <fnt>
          <bib>MT</bib>
          <lok>&Eli; 1:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni <tld/>is helpilojn, por subzoni la &scirc;ipon
        <fnt>
          <bib>NT</bib>
          <lok>&Ago; 27:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i siajn proprajn piedojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi posedis bototirilon kaj kombilon, kiujn mi neniam <tld/>is
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolumo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>is nenion el tiuj rajtoj
        <fnt>
          <bib>NT</bib>
          <lok>&Ko1; 9:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        anstata&ubreve; la vorto je ni povas anka&ubreve; <tld/>i akuzativon sen prepozicio
        <fnt>
          <bib>F</bib>
          <lok>&FE; 29</lok>
        </fnt>;
      </ekz>
      <ekz>
        la artikolo &#x201A;la&#8216; estas <ind><tld/>ata</ind> tiam, kiam ni
        parolas pri personoj a&ubreve; objektoj
        konataj
        <fnt>
          <bib>F</bib>
          <lok>&FE; 27</lok>
        </fnt>;
        <trd lng="en">used</trd>
        <trdgrp lng="id">
          <trd><baz>guna</baz>digunakan</trd>,
          <trd><baz>pakai</baz>dipakai</trd>,
          <trd><baz>pakai</baz>terpakai</trd>
        </trdgrp>
        <trd lng="fy">br&ucirc;kt</trd>
        <trd lng="nl">gebruikt</trd>
      </ekz>
      <ekz>
        <klr>[la &jcirc;urnalistoj]</klr> <tld/>is la imagon por elverki artikolojn kun
        frapantaj titoloj
        <fnt>
          <bib>KKl</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>is la saman pretekston
        <fnt>
          <bib>ChB</bib>
        </fnt>;
      </ekz>
      <ekz>
         <ind><tld/>i monon kaj admonon kaj flaton kaj baton</ind>
         <klr>(&ccirc;iujn rimedojn)</klr>
         <trd lng="fr">faire des pieds et des mains</trd>
         <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        &scirc;losilo <tld/>ata ruston ne konas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
     </dif>
    <refgrp tip="sin">
      <ref cel="aplik.0i.rimedon">apliki<sncref/></ref>,
      <ref cel="util.0igi.apliki">utiligi<sncref/></ref>
    </refgrp>
  <trdgrp lng="de">
    <trd>benutzen</trd>,
    <trd>gebrauchen</trd>
  </trdgrp></snc>
  <snc mrk="uz.0i.konsumi">
    <dif>
      <ref tip="super" cel="konsum.0i">Konsumi</ref> por
      kontentigi bezonon:
      <ekz>
        <tld/>u iom da vino pro la stomako
        <fnt>
          <bib>NT</bib>
          <lok>&Ti1; 5:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        la princo <tld/>is tri tagojn por &ccirc;i tiu voja&gcirc;o
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro 24a</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni <tld/>u bone la vivon, &ccirc;ar la vivo ne estas longa
        <fnt>
          <bib>F</bib>
          <lok>&FE; 20</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="uz.el0i">eluzi</ref>,
      <ref cel="uz.for0i">foruzi</ref>,
      <ref cel="gxu.0i">&gcirc;ui</ref>
    </refgrp>
    <trdgrp lng="de">
