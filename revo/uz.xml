<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>uz</rad>/i
</kap>
<drv mrk="uz.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Preni ion kiel rimedon por atingi celon; agi per io por kontentigi
      bezonon:
      <ekz>
        <tld/>i fingrojn anstata&ubreve; forketo;
      </ekz>
      <ekz>
        <tld/>i multe da salo<fnt>B</fnt>,
      </ekz>
      <ekz>
        po&scirc;an tran&ccirc;ilon<fnt>Z</fnt>,
      </ekz>
      <ekz>
        pafilegon<fnt>Z</fnt>,
      </ekz>
      <ekz>
        la artikolon<fnt>Z</fnt>,
      </ekz>
      <ekz>
        fina&jcirc;on<fnt>Z</fnt>,
      </ekz>
      <ekz>
        prepozicion<fnt>Z</fnt>,
      </ekz>
      <ekz>
        akuzativon;
      </ekz>
      <ekz>
        <tld/>i sian prudenton, rajton, artifikon;
      </ekz>
      <ekz>
        <tld/>i monon kaj admonon kaj flaton kaj baton<fnt>Z</fnt>
        <klr>(&ccirc;iujn rimedojn)</klr>;
      </ekz>
      <ekz>
        <tld/>u tempon estantan<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ni <tld/>u bone la vivon<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="gxu.0i">&gcirc;ui</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">pou&zcaron;&iacute;t</trd>
  <trdgrp lng="de">
    <trd>benutzen</trd>,
    <trd>gebrauchen</trd>
  </trdgrp>
  <trd lng="en">use</trd>
  <trd lng="fr">utiliser</trd>
  <trd lng="nl">gebruiken</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_p;&c_o;&c_t;&c_r;&c_je;&c_b;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_s;&c_p;&c_o;&c_l;&c_mol;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">anv&auml;nda</trd>
  <trd lng="tr">kullanmak</trd>
</drv>

<drv mrk="uz.0ado">
  <kap><tld/>(ad)o</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        la <tld/>ado de la artikolo estas sama kiel en la aliaj
        lingvoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        fari <tld/>on el prepozicio, el &ccirc;iu momento<fnt>Z</fnt>,
        de siaj armiloj;
      </ekz>
      <ekz>
        komuna &gcirc;isnuna <tld/>ado devas ludi en lingvo pli
        gravan rolon ol seke teoria logikeco<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Gebrauch</trd>
  <trd lng="fr">usage</trd>
  <trd lng="nl">gebruik</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_p;&c_o;&c_t;&c_r;&c_je;&c_b;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_i;&c_s;&c_p;&c_o;&c_l;&c_mol;&c_z;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sv">anv&auml;ndning</trd>
</drv>

<drv mrk="uz.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      &Ccirc;io, kion oni povas <tld/>i
    </dif>
    <refgrp tip="sin">
      <ref cel="il">ilo</ref>,
      <ref cel="instru2.0o">instrumento</ref>,
      <ref cel="aparat.0o">aparato</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">gebruiksvoorwerp</trd>
</drv>

<drv mrk="uz.el0i0difekti">
  <kap>el<tld/>i, <tld/>difekti</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Difekti per longa <tld/>ado:
      <ekz>
        la infano el<tld/>as multajn &scirc;uojn;
      </ekz>
      <ekz>
        el<tld/>ita pantalono;
      </ekz>
      <ekz>
        malnova el<tld/>ita&jcirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        okuloj el<tld/>itaj pro sen&ccirc;esa legado;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        el<tld/>i siajn fortojn per vanaj penoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">abnutzen</trd>
  <trd lng="fr">user</trd>
  <trd lng="nl">verslijten</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_p;&c_o;&c_l;&c_mol;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_z;&c_n;&c_o;&c_s;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_s;&c_t;&c_r;&c_je;&c_p;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv"><ind>slita</ind> ut</trd>
</drv>

<drv mrk="uz.for0i0konsumi">
  <kap>for<tld/>i, <tld/>konsumi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Konsumi per uzado:
      <ekz>
        oni for<tld/>is la tutan provizon da ligno dum la lasta vintro.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">aufbrauchen</trd>
  <trd lng="fr">&eacute;puiser</trd>
  <trd lng="nl">verbruiken</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_p;&c_o;&c_l;&c_mol;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_z;&c_n;&c_o;&c_s;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_z;&c_r;&c_a;&c_s;&c_h;&c_o;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_s;&c_t;&c_r;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="uz.fusx0i">
  <kap>fu&scirc;<tld/>i</kap>
  <snc>
    <dif>
      Difekti uzante, malbone uzi.
    </dif>
  </snc>
  <trd lng="fr">d&eacute;glinguer</trd>
</drv>

<drv mrk="uz.mis0i">
  <kap>mis<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="U"/>i en maniero, kiu ne estas &gcirc;usta:
      <ekz>
        mis<tld/>i sian a&ubreve;toritaton por defendi malvirtulojn.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">missbrauchen</trd>
  <trd lng="nl">misbruiken</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_l;&c_o;&c_u;&c_p;&c_o;&c_t;&c_r;&c_je;&c_b;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_je;&c_v;&c_je;&c_r;&c_n;&c_o; &c_i;&c_s;&c_p;&c_o;&c_l;&c_mol;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="uz.tro0i">
  <kap>tro<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld/>i super la &gcirc;usta, ordinara mezuro:
      <ekz>
        tro<tld/>i la kunmetitajn tempojn<fnt>B</fnt>,
      </ekz>
      <ekz>
        ies paciencon<fnt>B</fnt>
        (<refgrp tip="vid">
          <ref cel="ung.0ego">ungego</ref>,
          <ref cel="hauxt.sen0igi">senha&ubreve;tigi</ref>.
        </refgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">abuser de</trd>
  <trd lng="nl">misbruik maken</trd>
  <trd lng="ru">&c_z;&c_l;&c_o;&c_u;&c_p;&c_o;&c_t;&c_r;&c_je;&c_b;&c_l;&c_ja;&c_t;&c_mol;</trd>
</drv>

<drv mrk="uz.frukt0o">
  <kap>frukt<tld/>o</kap>
  <snc>
    <dif>
      Rajto <tld/>i ion, kies proprumecon havas alia, kaj kolekti &gcirc;iajn
      fruktojn a&ubreve; produktojn, sed ne &gcirc;in vendi a&ubreve; detrui.
    </dif>
  </snc>
  <trd lng="de">Nie&szlig;brauch</trd>
  <trd lng="fr">usufruit</trd>
  <trd lng="nl">vruchtgebruik</trd>
</drv>
</art>
