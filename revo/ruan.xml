<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ruan.xml,v 1.6 2024/04/15 20:21:39 revo Exp $">
<kap>
  <rad>ruan</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="ruan.0a">
  <kap><tld/>a</kap>
  <snc mrk="ruan.0a.KOL">
    <uzo tip="klr">(precipe pri besta felo)</uzo>
    <dif>
      De har<ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">koloro</ref>
      el miksitaj blankaj, ru&gcirc;e-brunaj, nigraj eroj:
      <ekz>
        li abrupte forigis de la frunto tufon de la
        longaj <tld/>aj haroj
        <fnt>
          <aut>K. Buli&ccirc;ov, trad. E. Tikhonov</aut>
          <vrk><url ref="http://sxakludanto.narod.ru/la_setlejo.htm">
          La setlejo</url></vrk>
          <lok>[2011?]</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia patro <klr>[&#8230;]</klr> havis bonan <tld/>an &ccirc;evalon kun longaj kolo kaj gamboj
        <fnt>
          <aut>Ivan Turgenev</aut>
          <vrk><url ref="https://esperanto-ondo.ru/Rn-turg1.htm"
              >La Unua Amo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Nu, kio do, &parsl; Ruanan mi a&ccirc;etos &parsl; Pro &gcirc;i ne multan monon oni petas
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 6 (56)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bej1.0a.KOL">beja</ref>,
      <ref cel="blond.0a.hararo">blonda</ref>,
      <ref cel="brun.0a">bruna</ref>,
      <ref cel="pig.0a.KOL">piga</ref>,
      <ref cel="ruf.0a.KOL">rufa</ref>
    </refgrp>
    <bld
      lok="&WCU;/thumb/a/ad/Laufender_Schecke.JPG/640px-Laufender_Schecke.JPG"
      prm="CC BY-SA 3.0"
      ><ind><tld/>a &ccirc;evalo</ind>, insulo Juist
        <fnt>
          <aut>4028mdk09, propra verko</aut>
          <lok><url ref="&WCW;/File:Laufender_Schecke.JPG"
              >https://commons.wikimedia.org/wiki/File:Laufender_Schecke.JPG</url></lok>
        </fnt>
        <trd lng="de">Schecke</trd>
        <trd lng="fr">pie <klr>(cheval)</klr></trd>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ja;&c_r;&c_e;&c_s;&c_t;&c_y;</trd>,
    <trd>&c_r;&c_a;&c_b;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gesprenkelt</trd>,
    <trd>gescheckt</trd>,
    <trd>meliert</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>sel et poivre</trd>,
    <trd>rouan</trd>
  </trdgrp>
  <trd lng="pl">dereszowaty  <klr>(o koniu)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_ja;&c_b;&c_i;&c_j;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_s;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>

