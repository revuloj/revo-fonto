<vortaro>
<art mrk="$Id: katedr.xml,v 1.21 2023/12/03 08:00:25 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>katedr</rad>/o
</kap>
<drv mrk="katedr.0o">
  <kap><tld/>o</kap>
  <snc mrk="katedr.0o.MEB">
    <dif>
      Surpodia se&gcirc;o por preleganto, predikanto, profesoro...:
      <ekz>
        suririnte sur la <tld/>on, li ne faru grimacon
        <fnt><bib>Revizoro</bib>, <lok>akto 1a, sceno 1a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>pulpit</trd>,
      <trd>throne</trd>,
      <trd>lecturer's desk</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>p&uacute;lpito con asiento</trd>,
      <trd>c&aacute;tedra</trd>
    </trdgrp>
    <trdgrp lng="nl">
    <trd>katheder</trd>
    </trdgrp>
    <trd lng="pl">katedra</trd>
  </snc>
  <snc mrk="katedr.0o.FIG">
    <uzo tip="fak">PED</uzo>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Profesora posteno:
      <ekz>
        vokis min la Universitato en Neuch&acirc;tel por teni la <tld/>on pri
        angla literaturo
        <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro 55a, p.
        123a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>professorship</trd>,
      <trd>university post</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>cargo universitario de profesor</trd>,
      <trd>c&aacute;tedra</trd>
    </trdgrp>
    <trdgrp lng="nl">
    <trd>leerstoel</trd>
    </trdgrp>
    <trd lng="pl">katedra</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_f;&c_je;&c_d;&c_r;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_t;&c_e;&c_d;&c_r;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>katedra</trd>,
    <trd>stolek</trd>,
    <trd>stolice</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Katheder</trd>,
    <trd>Lehrstuhl</trd>
  </trdgrp>
  <trd lng="fr">chaire</trd>
  <trdgrp lng="ja">
    <trd>&#x5E2D;  <pr>&#x3080;&#x3057;&#x308D;</pr></trd>,
    <trd>&#x8B1B;&#x58C7;  <pr>&#x3053;&#x3046;&#x3060;&#x3093;</pr></trd>,
    <trd>&#x8B1B;&#x5EA7;  <pr>&#x3053;&#x3046;&#x3056;</pr></trd>,
    <trd>&#x8AAC;&#x6559;&#x58C7;  <pr>&#x305B;&#x3063;&#x304D;&#x3087;&#x3046;&#x3060;&#x3093;</pr></trd>,
    <trd>&#x53F8;&#x6559;&#x5EA7;  <pr>&#x3057;&#x304D;&#x3087;&#x3046;&#x3056;</pr></trd>,
    <trd>&#x6CD5;&#x5EA7;  <pr>&#x307B;&#x3046;&#x3056;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_f;&c_je;&c_d;&c_r;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>katedra</trd>,
    <trd>stolica</trd>,
    <trd>stol&#xED;k</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&#x430;&#x444;&#x435;&#x434;&#x440;&#x430;</trd>
  <trdgrp lng="zh">
    <trd>&#x8B1B;&#x58C7; <pr>ji&#x1CE;ngt&#xE1;n</pr></trd>,
    <trd>&#x8B1B;&#x81FA; <pr>ji&#x1CE;ngt&#xE1;i</pr></trd>
  </trdgrp>
</drv>
</art>
