<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>1</ofc>
   <rad>katedr</rad>/o
</kap>
<drv mrk="katedr.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">PED</uzo>
   <snc mrk="katedr.0o.meblo">
     <dif>
       Surpodia se&gcirc;o por predikantoj a&ubreve; profesoroj.
     </dif>
   </snc>
   <snc mrk="katedr.0o.FIG">
     <uzo tip="stl">FIG</uzo>
     <dif>
       Profesora posteno:
       <ekz>
         vokis min la Universitato en Neuch&acirc;tel por teni la katedron pri
         angla literaturo
         <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro 55a, p. 123a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_k;&c_a;&c_f;&c_je;&c_d;&c_r;&c_a;</trd>,
     <trd>&c_k;&c_a;&c_t;&c_e;&c_d;&c_r;&c_a;</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>Katheder</trd>,
     <trd>Lehrstuhl</trd>
   </trdgrp>
   <trd lng="fr">chaire</trd>
   <trdgrp lng="nl">
     <trd>leerstoel</trd>,
     <trd>katheder</trd>
   </trdgrp>
   <trd lng="ru">&c_k;&c_a;&c_f;&c_je;&c_d;&c_r;&c_a;</trd>
</drv>
</art>
