<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>* 7</ofc><rad>ko</rad>
</kap>
<drv mrk="k.0">
  <kap><tld/></kap>
  <uzo tip="fak">LIN</uzo>
  <uzo tip="fak">GRA</uzo>
  <uzo tip="fak">FON</uzo>
  <snc mrk="k.0.konsonanto">    
   <dif>
      Nomo de la dekkvina litero de la
      Esperanta alfabeto, uzata i.a. kiel:
    </dif>
    <subsnc>
      <uzo tip="fak">KEM</uzo>
      <dif>
      (maj.) simbolo de kalio: <ekz>K</ekz>;
      </dif>
      <trd lng="fr">potassium</trd>
    </subsnc>
    <subsnc>
      <uzo tip="fak">FIZ</uzo>
      <dif>
      (maj.) simbolo de kelvino:<ekz> 273&deg;K</ekz>;
      </dif>
      <trd lng="fr">Kelvin</trd>
    </subsnc>
    <subsnc>
      <uzo tip="fak">FIZ</uzo>
      <dif>
      (min.) simbolo de kilo: <ekz>k</ekz>;
      </dif>
      <trd lng="fr">kilo</trd>
    </subsnc>
    <subsnc>
      <uzo tip="fak">TIP</uzo>
      <dif>
      (min.) mallongigo de kaj: <ekz>k.</ekz>;
      </dif>
      <trd lng="fr">et<klr> (abr&eacute;viation)</klr></trd>
    </subsnc>
  </snc>
  <trd lng="cs">k</trd>
  <trd lng="sk">k <klr>(kilo)</klr></trd>
</drv>

</art>

