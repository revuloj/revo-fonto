<vortaro>
<art mrk="$Id: riad.xml,v 1.8 2023/09/03 07:53:11 revo Exp $">
<kap>
  <rad>Riad</rad>/o
  <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="riad.0o">
  <kap><tld/>o</kap>
  <snc mrk="riad.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&Ccirc;efurbo</ref>
      de <ref tip="malprt" cel="arab.SaudaA0ujo">Sauda
      Arabujo</ref>:
      <ekz>
        <tld/>o situas centre de la lando, tamen iom pli proksime al Persa Golfo.
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=164329">Riado</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Bin Laden</nom> estis filo de ri&ccirc;a pia familio en Sauda Arabio naski&gcirc;inte en
        <tld/>o, lia patro de jemena deveno estis la plej ri&ccirc;a konstruisto de Sauda
        Arabio, sed mortis kiam <nom>Osama</nom> havis 13 jarojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3228">Usama bin Laden</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Riad</trd>
  </snc>
  <trd lng="be">&c_E;&c_r;-&c_R;&c_y;&c_ja;&c_d;</trd>
  <trdgrp lng="ca">
    <trd>Riad</trd>,
    <trd>Al-Riy&amacron;d</trd>
  </trdgrp>
  <trd lng="fr">Riyad</trd>
  <trd lng="ja">&#x30EA;&#x30E4;&#x30C9;</trd>
  <trd lng="pl">Rijad</trd>
  <trd lng="sv">Riyadh</trd>
  <trd lng="uk">&#x415;&#x440;-&#x420;&#x456;&#x44F;&#x434; (&#x441;&#x442;&#x43E;&#x43B;&#x438;&#x446;&#x44F;)</trd>
</drv>
</art>

