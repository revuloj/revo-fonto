<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc><rad>aflikt</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="aflikt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="aflikt.0i.PSI">
    <dif>
      Ka&ubreve;zi preman moralan suferon;
      <ref tip="vid" cel="prem.kor0i.PSI">korpremi</ref>,
      <ref tip="sin" cel="cxagre.0i.aflikti">&ccirc;agreni<sncref/></ref>:
      <ekz>
        lia mensogo kaj perfido <tld/>is &scirc;in.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_s;&c_m;&c_u;&c_ch;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>kormoutit</trd>,
    <trd>mrzet</trd>,
    <trd>rmoutit</trd>,
    <trd>soužit</trd>,
    <trd>trápit</trd>,
    <trd>tížit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>betr&uuml;ben</trd>,
    <trd>bek&uuml;mmern</trd>,
    <trd>kr&auml;nken</trd>,
    <trd>qu&auml;len</trd>
  </trdgrp>
  <trd lng="en">afflict</trd>
  <trd lng="es">afligir</trd>
  <trdgrp lng="fr">
    <trd>affliger</trd>,
    <trd>accabler<klr tip="ind"> (affliger)</klr></trd>,
    <trd>affecter<klr tip="ind"> (peiner)</klr></trd>,
    <trd>atteindre<klr tip="ind"> (peiner)</klr></trd>,
    <trd>attrister</trd>,
    <trd>chagriner<klr tip="ind"> (affliger)</klr></trd>,
    <trd>d&eacute;soler</trd>,
    <trd>navrer</trd>,
    <trd>peiner<klr tip="ind"> (chagriner)</klr></trd>,
    <trd>toucher<klr tip="ind"> (peiner)</klr></trd>
  </trdgrp>
  <trd lng="it">affliggere</trd>
  <trd lng="nl">bedroeven</trd>
  <trdgrp lng="pt">
    <trd>afligir</trd>,
    <trd>angustiar</trd>,
    <trd>acabrunhar</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_g;&c_o;&c_r;&c_ch;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>kormútiť</trd>,
    <trd>su&zcaron;ova&tcaron;</trd>,
    <trd>zarmucovať</trd>
  </trdgrp>
</drv>

<drv mrk="aflikt.0o">
  <kap><tld/>o</kap>
  <snc mrk="aflikt.0o.PSI">
    <dif>
      Sento kaj stato de iu <tld/>ita:
      <ekz>
        mi ne volas fari al mi <tld/>on pro tio<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxagre.0o">&ccirc;agreno</ref>,
      <ref cel="gxoj.mal0o">mal&gcirc;ojo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_s;&c_m;&c_u;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>sklíčenost</trd>,
    <trd>soužení</trd>,
    <trd>trápení</trd>,
    <trd>zármutek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Betr&uuml;bnis</trd>,
    <trd>K&uuml;mmernis</trd>,
    <trd>Kummer</trd>,
    <trd>Leid</trd>,
    <trd>Herzeleid</trd>,
    <trd>Niedergeschlagenheit</trd>,
    <trd>Qual</trd>
  </trdgrp>
  <trd lng="en">affliction</trd>
  <trdgrp lng="fr">
    <trd>affliction</trd>,
    <trd>accablement</trd>,
    <trd>chagrin<klr tip="ind"> (sentiment)</klr></trd>,
    <trd>peine<klr tip="ind"> (affliction)</klr></trd>,
    <trd>tristesse</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>afflizione</trd>,
    <trd>pena<klr> (sofferenza)</klr></trd>,
    <trd>travaglio</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>afli&ccedil;&atilde;o</trd>,
    <trd>agonia</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_g;&c_o;&c_r;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>s&uacute;&zcaron;enie</trd>,
    <trd>zármutok</trd>
  </trdgrp>
</drv>

<drv mrk="aflikt.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="aflikt.0igxi.PSI">
    <dif>
      I&gcirc;i <tld/>ata.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>se <ind>chagriner</ind><klr tip="ind">
    (s'affliger)</klr></trd>,
    <trd>s'<ind>affliger</ind></trd>
    <trd>s'<ind>attrister</ind></trd>,
    <trd>se <ind>d&eacute;soler</ind></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>affliggersi</trd>, <trd>addolorarsi</trd>
  </trdgrp>
</drv>

<drv mrk="aflikt.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc mrk="aflikt.0igxo.PSI">
    <ref tip="dif" cel="aflikt.0o"><tld/>o</ref>
  </snc>
  <trdgrp lng="fr">
    <trd>affliction</trd>,
    <trd>accablement</trd>,
    <trd>chagrin<klr tip="ind"> (sentiment)</klr></trd>,
    <trd>peine<klr tip="ind"> (affliction)</klr></trd>,
    <trd>tristesse</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>afflizione</trd>, <trd>sconforto</trd>
  </trdgrp>
</drv>
</art>
