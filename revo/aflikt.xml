<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap><ofc>3</ofc><rad>aflikt</rad>/i</kap>

<drv mrk="aflikt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Morale suferigi: 
      <ekz>lia mensogo kaj perfido <tld/>is &scirc;in.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_s;&c_m;&c_u;&c_ch;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
     <trd>betr&uuml;ben</trd>,
     <trd>bek&uuml;mmern</trd>,
     <trd>kra&auml;nken</trd>,
     <trd>qu&auml;len</trd>
  </trdgrp>
  <trd lng="es">afligir</trd>
  <trd lng="nl">bedroeven</trd>
  <trdgrp lng="pt">
    <trd>afligir</trd>,
    <trd>angustiar</trd>,
    <trd>acabrunhar</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_g;&c_o;&c_r;&c_ch;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="aflikt.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Stato de tiu, kiu estas <tld/>ita: 
      <ekz>mi ne volas fari al  mi <tld/>on pro tio<fnt>Z</fnt>.</ekz> 
    </dif>
    <trdgrp lng="pt">
      <trd>afli&ccedil;&atilde;o</trd>,
      <trd>agonia</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Betr&uuml;bnis</trd>,
      <trd>K&uuml;mmernis</trd>,
      <trd>Kummer</trd>,
      <trd>Leid</trd>,
      <trd>Herzeleid</trd>,
      <trd>Niedergeschlagenheit</trd>,
      <trd>Qual</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <dif>Ago <tld/>i.</dif>
    <refgrp tip="sin">
      <ref cel="cxagre">&ccirc;agreno</ref>,
      <ref cel="gxoj">mal&gcirc;ojo</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Betr&uuml;ben</trd>,
      <trd>Kra&auml;nken</trd>
    </trdgrp>
    <trd lng="pt">afli&ccedil;&atilde;o</trd>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_s;&c_m;&c_u;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="it">affliggere</trd>
  <trd lng="ru">&c_o;&c_g;&c_o;&c_r;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>
</art>
