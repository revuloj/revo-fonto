<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>genist</rad>/o
</kap>
<drv mrk="genist.0o">
  <kap><tld/>o</kap>
  <snc mrk="genist.0o.BOT">
  <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Genista</trd>),
      arbedo kun flavaj floroj:
      <ekz>
        li ku&scirc;i&gcirc;is kaj endormi&gcirc;is sub la <tld/>o
        <fnt><bib>MT</bib>, <lok>&Re1; 19:5</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_w;&c_t;&c_a;&c_z;&c_je;&c_l;&c_mol;</trd>
  <trd lng="de">Ginster</trd>
  <trd lng="fr">gen&ecirc;t</trd>
  <trd lng="hu">rekettye</trd>
  <trd lng="nl">brem</trd>
  <trd lng="pt">giesta</trd>
  <trd lng="ru">&c_d;&c_r;&c_o;&c_k;</trd>
</drv>
</art>
