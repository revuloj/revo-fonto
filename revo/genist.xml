<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: genist.xml,v 1.25 2025/08/11 16:41:41 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>genist</rad>/o
<fnt><bib>PVEF</bib></fnt></kap>
<drv mrk="genist.0o">
  <kap><tld/>o</kap>
  <snc mrk="genist.0o.BOT">
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Genista</trd>),
      arbedo kun flavaj floroj:
      <ekz>
        li ku&scirc;i&gcirc;is kaj endormi&gcirc;is sub la <tld/>o
        <fnt><bib>MT</bib>, <lok>&Re1; 19:5</lok></fnt>;
      </ekz>
      <ekz>
        la montetojn kovris la malpalaj flavaj floroj de la <tld/>o
        <fnt>
          <bib>StU</bib>
          <lok>Al Romo</lok>
        </fnt>;
      </ekz>
      <ekz>
        karboj <tld/>aj
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 120:4</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="uleks.0o">ulekso</ref>
    <bld
      lok="&WCU;/thumb/3/3f/Genista_salzmannii01.jpg/640px-Genista_salzmannii01.jpg"
      prm="CC BY 2.5"
      >floranta bran&ccirc;o de <tld/>o (Genista salzmannii) en Korsiko
        <fnt>
          <aut>User:Tigerente, propra verko</aut>
          <lok><url ref="&WCW;/File:Genista_salzmannii01.jpg"
              >https://commons.wikimedia.org/wiki/File:Genista_salzmannii01.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_w;&c_t;&c_a;&c_z;&c_je;&c_l;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>argelaga</trd>,
    <trd>ginestola</trd>,
    <trd>ginestell</trd>,
    <trd>gaiol</trd>,
    <trd>g&egrave;nere Genista</trd>
  </trdgrp>
  <trd lng="cs">kru&ccaron;inka</trd>
  <trd lng="de">Ginster</trd>
  <trd lng="en">broom</trd>
  <trd lng="fr">gen&ecirc;t</trd>
  <trd lng="hu">rekettye</trd>
  <trd lng="ja">&#12498;&#12488;&#12484;&#12496;&#12456;&#12491;&#12471;&#12480;</trd>
  <trd lng="nl">brem</trd>
  <trd lng="pl">janowiec <klr>(ro&sacute;lina)</klr></trd>
  <trd lng="pt">giesta</trd>
  <trd lng="ru">&c_d;&c_r;&c_o;&c_k;</trd>
  <trd lng="sk">kru&ccaron;ina</trd>
  <trd lng="uk">&c_d;&c_r;&c_ib;&c_k;</trd>
  <trd lng="zh">&#37329;&#38592;&#33457; <pr>j&imacron;nqu&egrave;hu&amacron;</pr></trd>
</drv>
</art>
