   <trd lng="ru">&c_v;-&c_d;&c_je;&c_v;&c_ja;&c_t;&c_y;&c_h;</trd>
  <trd lng="zh">&#x7B2C;&#x4E5D; <pr>d&#xEC;ji&#x1D4;</pr></trd>
</drv>
