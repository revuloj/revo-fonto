   <trd lng="nl">negende</trd>
   <trd lng="pl">dziewi&aogonek;ty</trd>
   <trd lng="ru">&c_d;&c_je;&c_v;&c_ja;&c_t;&c_y;&c_j;</trd>
   <trd lng="uk">&c_d;&c_je;&c_v;&#x2019;&c_ja;&c_t;&c_i;&c_j;</trd>
  <trd lng="zh">&#x7B2C;&#x4E5D; <pr>d&igrave;ji&#x1D4;</pr></trd>
</drv>

<drv mrk="naux.0e">
   <kap><tld/>e</kap>
   <snc>
     <dif>
       <tld lit="N"/>arange, post ok anta&ubreve;aj:
      <ekz>
        unue, kvankam sonas altagrade morale kondamni &ccirc;iujn <klr>[&#x2026;]</klr>,
        oke, <klr>[&#x2026;]</klr> oni ne sukcesos nei <klr>[&#x2026;]</klr>, <tld/>e, <klr>[&#x2026;]</klr>
        mi bon&scirc;ance ricevis malpli unuflankan bildon pri la okazinta&jcirc;oj
        <fnt>
	<aut>Uwe Joachim Moritz/Aldo de' Giorgi</aut>
	<vrk><url ref="&Monato;/2000/007043.php">Du opinioj</url></vrk>
	<bib>Monato</bib>
	<lok>2000/07, p. 4</lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_p;&c_a;-&c_d;&c_z;&c_ja;&c_v;&c_ja;&c_t;&c_a;&c_je;</trd>
   <trd lng="br">da <ind>navvet</ind></trd>
