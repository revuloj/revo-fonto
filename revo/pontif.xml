<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: pontif.xml,v 1.16 2023/09/03 12:45:10 revo Exp $">
    <kap>
      <rad>pontifik</rad>/o
      <fnt>Z</fnt>
    </kap>

    <drv mrk="pontif.0o">
      <kap><tld/>o</kap>
      <snc mrk="pontif.0o.HIS">
        <uzo tip="fak">HIS</uzo>
        <dif>
          En <ref tip="malprt" cel="romi.0o">Romio</ref>,
          ano de pagana kolegio profesia (prae: de la
          pontokonstruistoj, kiel poste la
          <ref cel="framas.0o">framasonoj</ref>), kies prezidanto i&gcirc;is
          estro de &ccirc;iuj kultoj:
          <ekz>
            la kolegio de la <tld/>oj<fnt>PV</fnt>.
          </ekz>
        </dif>
        <trd lng="be">&c_p;&c_a;&c_n;&c_t;&c_y;&c_f;&c_ib;&c_k;</trd>
        <trd lng="en">pontifex</trd>
        <trd lng="ru">&c_p;&c_o;&c_n;&c_t;&c_i;&c_f;&c_i;&c_k;</trd>
      </snc>
      <snc mrk="pontif.0o.KAT">
        <uzo tip="fak">KAT</uzo>
        <dif>
          Eklezia altrangulo havanta a&ubreve;toritaton kaj ju&gcirc;povon:
          <ekz>
            la <ref cel="pap.0o.KAT">papo</ref> estas la
            plej alta <tld/>o<fnt>B</fnt>.
          </ekz>
        </dif>
        <trd lng="be">&c_p;&c_je;&c_r;&c_sh;&c_a;&c_s;&c_mol;&c_v;&c_ja;&c_t;&c_a;&c_r;</trd>
        <trd lng="ru">&c_p;&c_je;&c_r;&c_v;&c_o;&c_s;&c_v;&c_ja;&c_shch;&c_je;&c_n;&c_n;&c_i;&c_k;</trd>
      </snc>
  <trdgrp lng="cs">
    <trd>biskup</trd>,
    <trd>pontifex</trd>,
    <trd>velekn&#x11B;z</trd>
  </trdgrp>
  <trd lng="de">Bischof</trd>
  <trd lng="hu">f&odblac;pap</trd>
  <trd lng="it">pontefice</trd>
  <trdgrp lng="ja">
    <trd>&#x5927;&#x795E;&#x5B98;  [<ind>&#x3060;&#x3044;&#x3057;&#x3093;&#x304B;&#x3093;</ind>]</trd>,
    <trd>&#x9AD8;&#x4F4D;&#x8056;&#x8077;&#x8005;  [<ind>&#x3053;&#x3046;&#x3044;&#x305B;&#x3044;&#x3057;&#x3087;&#x304F;&#x3057;&#x3083;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">pontifex</trd>
  <trd lng="pl">pontyfik</trd>
  <trd lng="pt">pont&iacute;fice</trd>
  <trd lng="ro">pontif</trd>
  <trdgrp lng="sk">
    <trd>biskup</trd>,
    <trd>pontifex</trd>,
    <trd>ve&#x13E;k&ncaron;az</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43D;&c_t;&c_i;&c_f;&c_ib;&#x43A;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_v;&#x43E;&c_s;&c_v;&#x44F;&c_shch;&c_je;&#x43D;&c_i;&#x43A;</trd>,
    <trd>&c_jeu;&#x43F;&c_i;&c_s;&#x43A;&#x43E;&#x43F;</trd>,
    <trd>&#x43F;&c_r;&c_je;&#x43B;&c_a;&c_t;</trd>
  </trdgrp>
</drv>
