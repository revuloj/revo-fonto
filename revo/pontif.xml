<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>pontifik</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="pontif.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      Eklezia altrangulo havanta a&ubreve;toritaton kaj ju&gcirc;povon:
      <ekz>
        la papo estas la plej alta <tld/>o<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Bischof</trd>
  <trd lng="hu">f&odblac;pap</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_n;&c_t;&c_i;&c_f;&c_i;&c_k;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_v;&c_o;&c_s;&c_v;&c_ja;&c_shch;&c_je;&c_n;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
