<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kvint.xml,v 1.22 2023/12/03 09:59:05 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>kvint</rad>/o
</kap>
<drv mrk="kvint.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="kvint.0o.intervalo">
    <dif>
      <ref tip="super" cel="interv.0o.MUZ">Intervalo</ref>
      inter la du ekstremaj el kvin sinsekvaj tonoj de gamo:
      <ekz>
        en Islando <klr>...</klr> konservi&gcirc;is <klr>...</klr> duvo&ccirc;a
        kanto, tielnomata <tld/>-kanto, &ccirc;ar plejparte la interspaco 
        inter la du vo&ccirc;oj estas <tld/>oj
        <fnt><aut>Stefan Sigurdsson</aut>, <vrk>fluge super la islanda
        kulturo</vrk>, <lok>norda prismo, 1956, numero 3a, p.
        104-106a</lok></fnt>;
      </ekz>
      <ekz>
        en malnovaj orgenoj norde de la Alpoj 12&minute; signifas foje anka&ubreve; <tld/>on  10<sup>2/3</sup>&minute;
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=227658">Registro (orgeno)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la pitagora <klr>[&#x2026;]</klr> <ind>lupokvinto</ind> havas la 
        frekvencproporcion (1/2)<sup>7</sup>:(2/3)<sup>11</sup>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=258745">Lupokvinto</url></lok>
        </fnt>.
        <trd lng="de">Wolfsquinte</trd>
      </ekz>
    </dif>
  </snc>
  <snc mrk="kvint.0o.de_toniko">
    <dif>
      Kvina tono de gamo.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_v;&c_ib;&c_n;&c_t;&c_a;</trd>
  <trd lng="bg">&c_k;&c_v;&c_i;&c_n;&c_t;&c_a;</trd>
  <trd lng="cs">kvinta</trd>
  <trd lng="de">Quinte</trd>
  <trd lng="en">fifth</trd>
  <trdgrp lng="fr">
    <trd>quinte<klr> (intervalle musical)</klr></trd>
  </trdgrp>
  <trd lng="hu">kvint</trd>
  <trd lng="ja">&#x4E94;&#x5EA6;&#x97F3;&#x7A0B;  <pr>&#x3044;&#x3064;&#x3069;&#x304A;&#x3093;&#x3066;&#x3044;</pr></trd>
  <trd lng="nl">quintool</trd>
  <trd lng="pl">kwinta</trd>
  <trd lng="pt">quinta <klr>(m&uacute;s.)</klr></trd>
  <trd lng="ru">&c_k;&c_v;&c_i;&c_n;&c_t;&c_a;</trd>
  <trd lng="sk">kvinta</trd>
  <trd lng="uk">&#x43A;&#x432;&#x456;&#x43D;&#x442;&#x430;</trd>
  <trdgrp lng="zh">
    <trd>&#x7D14;&#x4E94;&#x5EA6; <pr>ch&#xFA;nw&#x1D4;d&#xF9;</pr></trd>,
    <trd>&#x7EAF;&#x4E94;&#x5EA6; <pr>ch&#xFA;nw&#x1D4;d&#xF9;</pr></trd>
  </trdgrp>
</drv>
</art>
