<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>grejs</rad>/o
</kap>
<drv mrk="grejs.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Speco de roka &scirc;tono, konsistanta el silikaj grajnetoj aglomeritaj
      en kalkeca a&ubreve; silikeca maso. 
    </dif>
    <ref tip="sin" cel="sabl.0osxtono">sablo&scirc;tono.</ref>
    <refgrp tip="vid">
      Ne konfuzu kun:
      <ref cel="gnejs.0o">gnejso</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Sandstein</trd>
  <trd lng="fr">gr&egrave;s<klr tip="ind"> (roche)</klr></trd>
  <trd lng="hu">homokk&odblac;</trd>
  <trd lng="nl">zandsteen</trd>
  <trd lng="ru">&c_p;&c_je;&c_s;&c_ch;&c_a;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
