<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>rektif</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="rektif.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rektif.0i.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Purigi likva&jcirc;on per nova distilo a&ubreve; alia &hcirc;emia procedo.
    </dif>
    <trd lng="de">rektifizieren</trd>
    <trd lng="fr">rectifier<klr tip="ind"> (chimie)</klr></trd>
    <trd lng="hu">rektifik&aacute;l<klr tip="ind"> (k&eacute;mia)</klr></trd>    
    <trd lng="nl">zuiveren<klr tip="ind"> (door distillatie)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_je;&c_k;&c_t;&c_i;&c_f;&c_i;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_ch;&c_i;&c_shch;&c_a;&c_t;&c_mol;<klr tip="ind"> (&c_h;&c_i;&c_m;.)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="rektif.0i.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 38</lok></fnt>
    <uzo tip="stl">ARK</uzo>
    <dif>
      <klr>(<ref cel="kurb.0o.MAT">kurbon</ref>)</klr>
      Per simplaj desegnaj iloj konstrui strekon samlongan kiel donita
      segmento de la kurbo:
      <ekz>
        neeblas <tld/>i cirklon per rektilo kaj cirkelo.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>kiegyenes&iacute;t</trd>,
      <trd>rektifik&aacute;l</trd>
    </trdgrp>
  </snc>
  <snc mrk="rektif.0i.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Rektigi alternan kurenton, transformi &gcirc;in en rektan a&ubreve;
      (pli speciale) en konstantan kurenton.
    </dif>
    <trd lng="de">gleichrichten</trd>
    <trd lng="es">rectificar</trd>
    <trd lng="fr">redresser<klr tip="ind"> (&eacute;lectricit&eacute;)</klr></trd>
    <trd lng="hu">egyenir&aacute;ny&iacute;t</trd>    
    <trd lng="nl">gelijkrichten</trd>
    <trd lng="ru">&c_v;&c_y;&c_p;&c_r;&c_ja;&c_m;&c_l;&c_ja;&c_t;&c_mol; <klr tip="amb">(&c_e;&c_l;. &c_t;&c_o;&c_k;)</klr></trd>
  </snc>
</drv>

<drv mrk="rektif.0ebla">
  <kap><tld/>ebla</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>EKV</bib><lok>&para;374</lok></fnt>
  <snc mrk="rektif.0ebla.MAT">
    <dif>
      <klr>(p.p. <ref cel="kurb.0o.MAT">kurbo</ref>)</klr>
      Tia, ke eblas kalkuli &gcirc;ian longon.
    </dif>
    <trd lng="de">rektifizierbar<klr tip="ind"> (-e Kurve)</klr></trd>
    <trd lng="en">rectifiable<klr tip="ind"> (curve)</klr></trd>
    <trd lng="fr"><klr tip="ind">(courbe) </klr>rectifiable</trd>
    <trd lng="hu">kiegyenes&iacute;thet&odblac; <klr tip="ind">(g&ouml;rbe)</klr></trd>    
    <trdgrp lng="pl">
      <trd><klr tip="amb">(krzywa) </klr>prostowalna</trd>,
      <trd><klr tip="amb">(krzywa) </klr>rektyfikowalna</trd>
    </trdgrp> 
    <trd lng="ru">&c_s;&c_p;&c_r;&c_ja;&c_m;&c_l;&c_ja;&c_je;&c_m;&c_a;&c_ja;<klr tip="amb"> (&c_k;&c_r;&c_i;&c_v;&c_a;&c_ja;)</klr></trd>
  </snc>
</drv>

<drv mrk="rektif.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      <ref tip="sin" cel="rekt.0igilo">Rektigilo</ref>
      de alterna kurento.
    </dif>
  </snc>
  <trd lng="de">Gleichrichter</trd>
  <trd lng="es">rectificador</trd>
  <trd lng="fr">redresseur</trd>
  <trd lng="hu">egyenir&aacute;ny&iacute;t&oacute;</trd>  
  <trd lng="nl">gelijkrichter</trd>
  <trd lng="ru">&c_v;&c_y;&c_p;&c_r;&c_ja;&c_m;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
</drv>
</art>
