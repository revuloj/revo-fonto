<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>rektif</rad>/i
<fnt>Z</fnt>
</kap>
<drv mrk="rektif.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rektif.0i.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Purigi likva&jcirc;on per nova distilo a&ubreve; alia &hcirc;emia procedo.
    </dif>
    <trd lng="fr">rectifier<klr tip="ind"> (chimie)</klr></trd>
    <trd lng="nl">zuiveren<klr tip="ind"> (door distillatie)</klr></trd>
  </snc>
  <snc mrk="rektif.0i.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <uzo tip="stl">ARK</uzo>
    <dif>
      <klr>(<ref cel="kurb.0o.MAT">kurbon</ref>)</klr>
      Per simplaj desegnaj iloj konstrui strekon samlongan kiel donita
      segmento de la kurbo:
      <ekz>
        neeblas <tld/>i cirklon per rektilo kaj cirkelo.
      </ekz>
    </dif>
  </snc>
  <snc mrk="rektif.0i.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Transformi alternan kurenton en kontinuan a&ubreve; onduman kurenton.
    </dif>
    <trd lng="fr">redresser<klr tip="ind"> (&eacute;lectricit&eacute;)</klr></trd>
  <trd lng="nl">gelijkrichten</trd>
</snc>
</drv>
<drv mrk="rektif.0ebla">
  <kap><tld/>ebla</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc mrk="rektif.0ebla.MAT">
    <dif>
      <klr>(p.p. <ref cel="kurb.0o.MAT">kurbo</ref>)</klr>
      Tia, ke eblas kalkuli &gcirc;ian longon.
    </dif>
    <trd lng="en">rectifiable<klr tip="ind"> (curve)</klr></trd>
    <trd lng="de">rektifizierbare<klr tip="amb"> (Kurve)</klr></trd>
    <trd lng="fr"><klr tip="ind">(courbe) </klr>rectifiable</trd>
    <trd lng="ru">&c_s;&c_p;&c_r;&c_ja;&c_m;&c_l;&c_ja;&c_je;&c_m;&c_a;&c_ja;<klr tip="amb"> (&c_k;&c_r;&c_i;&c_v;&c_a;&c_ja;)</klr></trd>
  </snc>
</drv>
<drv mrk="rektif.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      Aparato por <tld/>i alternan kurenton.
    </dif>
  </snc>
  <trd lng="fr">redresseur</trd>
<trd lng="nl">gelijkrichter</trd>
</drv>
</art>

