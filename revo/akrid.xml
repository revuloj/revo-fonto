<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>akrid</rad>/o
</kap>
<drv mrk="akrid.0o">
  <kap><tld/>o</kap>
  <snc mrk="akrid.0o.KOMUNE">
    <dif>
      Iu el la familio <tld/>edoj, saltinsekto kun mallongaj antenoj
      kaj povanta muziki per frotado de la kruroj al la elitroj.
    </dif>
    <trd lng="de">Heuschrecke</trd>
    <trd lng="fr">criquet</trd>
     <trdgrp lng="pt">
        <trd>gafanhoto</trd>,
        <trd>grilo</trd>
     </trdgrp>
    <trd lng="sv">gr&auml;shoppa</trd>
  </snc>
  <snc mrk="akrid.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="akrid.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Acridium</trd>).
    </dif>
  </snc>
</drv>
<drv mrk="akrid.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo de
      <ref tip="super" cel="ortopter.0oj">ortopteroj</ref>
      (<trd lng="la">Acrididae</trd>),
      de saltantaj insektoj, kun multegaj specioj.
    </dif>
  </snc>
  <trd lng="de">Feldheuschrecken</trd>
  <trd lng="fr">acridiens</trd>
</drv>
</art>
