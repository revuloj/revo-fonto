<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>akrid</rad>/o
</kap>
<drv mrk="akrid.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="akrid.0o.KOMUNE">
    <dif>
      Iu el la familio <tld/>edoj, saltinsekto kun mallongaj antenoj
      kaj povanta muziki per frotado de la kruroj al la elitroj.
    </dif>
    <trd lng="be">&c_s;&c_a;&c_r;&c_a;&c_n;&c_ch;&c_a;</trd>
    <trd lng="de">Heuschrecke</trd>
    <trdgrp lng="br">
        <trd>karv-raden</trd>,
        <trd>kilhog-raden</trd>
    </trdgrp>
    <trdgrp lng="en">
        <trd>grasshopper</trd>,
        <trd>locust</trd>
     </trdgrp>
    <trd lng="fr">criquet</trd>
    <trd lng="hu">s&aacute;ska</trd>
    <trd lng="nl">sprinkhaan</trd>
    <trdgrp lng="pl">
      <trd>konik polny</trd>,
      <trd>szara&nacute;cza</trd>
    </trdgrp>
    <trdgrp lng="pt">
        <trd>gafanhoto</trd>,
        <trd>grilo</trd>
     </trdgrp>
    <trd lng="sv">gr&auml;shoppa</trd>
  </snc>
  <snc mrk="akrid.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super"
      cel="akride.0oj"><tld/>edoj</ref>
      (<trd lng="la">Acridium</trd>).
    </dif>
    <trdgrp lng="de">
      <trd>Feldheuschrecke</trd>,
      <trd>Heuh&uuml;pfer</trd>,
      <trd>Grille</trd>
    </trdgrp>
    <trd lng="en">Acridium</trd>
    <trd lng="pl">szara&nacute;cza</trd>
  </snc>
  <trd lng="ru">&c_s;&c_a;&c_r;&c_a;&c_n;&c_ch;&c_a;</trd>
  <trd lng="sk">kobylka <klr>(hmyz)</klr></trd>
</drv>

</art>
