<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: akrid.xml,v 1.36 2022/04/25 15:10:59 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>akrid</rad>/o
</kap>
<drv mrk="akrid.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="akrid.0o.KOMUNE">
    <dif>
      Iu el la familio <tld/>edoj, 
      salt<ref tip="lst" cel="insekt.0o" lst="voko:insektoj">insekto</ref> 
      kun mallongaj antenoj
      kaj povanta muziki per frotado de la kruroj &ccirc;e la elitroj:
      <ekz>
        etendu vian manon super la landon Egiptan pro la <tld/>oj, ke ili 
        venu sur la landon Egiptan, kaj forman&gcirc;u &ccirc;iujn herbojn de la tero 
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 10:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        la jenajn el ili vi povas man&gcirc;i: la <tld/>on kun &gcirc;ia speco 
        kaj la <ref cel="cikad.0o">cikadon</ref>
        kun &gcirc;ia speco kaj la <ref cel="gril.0o.familiano">grilon</ref>
        kun &gcirc;ia speco kaj la <ref cel="lokust.0o">lokuston</ref> kun &gcirc;ia speco
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 11:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        li man&gcirc;adis <tld/>ojn kaj sova&gcirc;an mielon 
        <fnt>
          <bib>NT</bib>
          <lok>Marko 1:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili venis kiel granda nubo, kiel svarmo da <tld/>oj 
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        fatala kombino de pluv-manko kaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2006/008814.php">E&ubreve;ropo plialtigas sian drataron</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_a;&c_r;&c_a;&c_n;&c_ch;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Kurzf&uuml;hlerschrecken</trd>,
      <trd>Heuschrecke <klr>(Kurzf&uuml;hler)</klr></trd>
    </trdgrp>
    <trdgrp lng="br">
        <trd>karv-raden</trd>,
        <trd>kilhog-raden</trd>
    </trdgrp>
    <trdgrp lng="en">
        <trd>grasshopper</trd>,
        <trd>locust</trd>
     </trdgrp>
    <trd lng="fr">criquet</trd>
    <trd lng="hu">s&aacute;ska</trd>
    <trd lng="id">belalang</trd>
    <trd lng="nl">sprinkhaan</trd>
    <trdgrp lng="pl">
      <trd>konik polny</trd>,
      <trd>szara&nacute;cza</trd>
    </trdgrp>
    <trdgrp lng="pt">
        <trd>gafanhoto</trd>,
        <trd>grilo</trd>
     </trdgrp>
    <trd lng="sv">gr&auml;shoppa</trd>
  </snc>
  <snc mrk="akrid.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio
      <ref tip="malprt" cel="akride.0oj"><tld/>edoj</ref>
      (<trd lng="la">Acrida</trd>).
    </dif>
    <bld lok="&WCU;/thumb/2/2d/Acrida_hungarica_%2822123663930%29.jpg/640px-Acrida_hungarica_%2822123663930%29.jpg" prm="CC BY-SA 2.0"><ind>hungara <tld/>o</ind> (Acrida hungarica)
        <fnt>
          <aut>Gilles San Martin de Namur, Belgujo</aut>
          <lok><url ref="&WCW;/File:Acrida_hungarica_(22123663930).jpg">&WCW;/File:Acrida_hungarica_(22123663930).jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="de">
      <trd>Feldheuschrecke</trd>,
      <trd>Grash&uuml;pfer</trd>,
      <trd>Heuh&uuml;pfer</trd>,
      <trd>Grille</trd>
    </trdgrp>
    <trd lng="en">Acridium</trd>
    <trd lng="pl">szara&nacute;cza</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>kobylka lu&ccaron;n&iacute;</trd>,
    <trd>saran&ccaron;e</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>acridien</trd>,
    <trd>acridid&eacute;</trd>
  </trdgrp>
  <trd lng="he">&#x5D0;&#x5E8;&#x5D1;&#x5D4;</trd>
  <trd lng="ru">&c_s;&c_a;&c_r;&c_a;&c_n;&c_ch;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>kobylka</trd>,
    <trd>kobylka <klr>(hmyz)</klr></trd>,
    <trd>saran&ccaron;a</trd>
  </trdgrp>
  <trd lng="uk">&#x441;&#x430;&#x440;&#x430;&#x43D;&#x430;</trd>
</drv>

</art>
