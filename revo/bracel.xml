<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>bracelet</rad>/o
</kap>
<drv mrk="bracel.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ornama&jcirc;o portata &ccirc;irka&ubreve; la manradiko, la brako, a&ubreve; la kruro.
    </dif>
    <refgrp tip="sub">
      <ref cel="man.cxirkaux0o">&ccirc;irka&ubreve;mano</ref>,
      <ref cel="brak.0ringo">brakringo</ref>,
      <ref cel="krur.0ringo">krurringo</ref>.
    </refgrp>
  </snc>                         
  <trd lng="be">&c_b;&c_r;&c_a;&c_n;&c_z;&c_a;&c_l;&c_je;&c_t;</trd>
  <trd lng="br">tro-vrec'h</trd>
  <trd lng="es">brazalete</trd>
  <trd lng="fr">bracelet</trd>
  <trd lng="hu">karperec</trd>
  <trd lng="nl">armband</trd>
  <trd lng="pl">bransoleta</trd>
  <trd lng="ru">&c_b;&c_r;&c_a;&c_s;&c_l;&c_je;&c_t;</trd>
  <trd lng="sk">n&aacute;ramok</trd>
</drv>
</art>
