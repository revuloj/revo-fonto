<vortaro>
  <art mrk="$Id: edinburg.xml,v 1.7 2025/02/28 18:37:33 afido Exp $">
    <kap><ofc>10</ofc>
      <rad>Edinburg</rad>/o
    </kap>

<drv mrk="edinburg.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&Ccirc;efurbo</ref> de
      <ref tip="malprt" cel="skot.0lando">Skotlando</ref>,
      70&#x202F;km oriente de <ref tip="malprt" cel="glasgov.0o">Glasgovo</ref>:
      situo:  (<frm>55&#x2DA;57&minute; N 3&#x2DA;11&minute; W</frm>):
      <ekz>
        sep Universalaj Kongresoj okazis en britaj urboj:
         (1907), <tld/>o
        (1926), Oksfordo (1930), Londono (1938), <klr>[&#8230;]</klr>
        kaj Londono (1971)
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        en <tld/>o estas miloj da domoj, kaj en &ccirc;iu domo devas
        trovi&gcirc;i ses el niaj
        diversspecaj brosoj mi-ni-mu-me
        <fnt>
          <bib>KrM</bib>
          <lok>3. Ne Gutas Mielo El La &Ccirc;ielo.</lok>
        </fnt>!
      </ekz>
      <ekz>
        70 homoj ari&gcirc;is en la Nacia Biblioteko en <tld/>o
        7 dec 2001 por celebri la
        malavaran donacon de la libraro de <nom>Bill</nom> (<nom>William</nom>) <nom>Auld</nom>
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470;1</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_E;&c_d;&c_y;&c_n;&c_b;&c_u;&c_r;&c_g;</trd>
    <trd lng="de">Edinburgh</trd>
    <trd lng="en">Edinburgh</trd>
    <trd lng="fr">&Eacute;dimbourg</trd>
    <trd lng="nl">Edinburgh</trd>
    <trd lng="gd">D&ugrave;n &Egrave;ideann</trd>
    <trd lng="pl">Edynburg</trd>
  </snc>
  <trd lng="ja">&#x30A8;&#x30B8;&#x30F3;&#x30D0;&#x30E9;</trd>
  <trd lng="zh">&#x611B;&#x4E01;&#22561; <pr>&#xE0;id&#x12B;ngb&#x1CE;o</pr></trd>
 </drv>
