<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lauxrac.xml,v 1.9 2019/11/03 23:10:24 revo Exp $">
<kap>
  <rad>la&ubreve;rac</rad>/oj
</kap>
<drv mrk="lauxrac.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> 
      de <ref tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Lauraceae</trd>):
      <ekz>
        anda alno kaj montara podokarpo je pli malaltaj altitudoj
        <klr>[&#8230;]</klr>
        miksi&gcirc;as kun aliaj arbospecioj, speciale <tld/>oj kaj mirtacoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=432182"
              >Sud-andaj jungaoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
       <ref cel="lauxr.0o">la&ubreve;ro</ref>,
       <ref cel="cinam.0ujo">cinamujo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_w;&c_r;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Lorbeergew&auml;chse</trd>
  <trd lng="el">&delta;&alfa;&phi;&ny;&omikron;&epsilon;&jota;&delta;&eta_ton;</trd>
  <trd lng="hu">bab&eacute;rf&eacute;l&eacute;k</trd>
  <trd lng="nl">laurierfamilie</trd>
  <trd lng="pl">wawrzynowate</trd>
  <trd lng="pt">laur&aacute;ceas</trd>
  <trd lng="ro">Lauraceae</trd>
  <trd lng="ru">&c_l;&c_a;&c_v;&c_r;&c_o;&c_v;&c_y;&c_je;<klr tip="ind">
(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
