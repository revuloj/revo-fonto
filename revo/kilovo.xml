<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kilovo.xml,v 1.18 2022/03/28 11:08:34 revo Exp $">
<kap>
  <rad>kilovolt</rad>/o
</kap>
<drv mrk="kilovo.0o">
  <kap><tld/>o</kap>
  <mlg kod="SI">kV</mlg>
  <uzo tip="fak">ELE</uzo>
  <fnt><bib>PV</bib></fnt>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:mezurunuoj" cel="unu.mezur0o">Mezurunuo</ref>
      de <ref cel="fort.elektromova0o">elektromova forto</ref>
      kaj <ref cel="tensi.0o.ELE">tensio</ref>, egala al
      mil <ref tip="prt" cel="volt.v0o">voltoj</ref>:
      <ekz>
        &ccirc;irka&ubreve; la lago ne nur konstrui&gcirc;is &scirc;oseoj kondukantaj al 
        &ccirc;iuj vila&gcirc;oj de la regiono, sed anka&ubreve; funkcias 35-<tld/>a
        transformatora substacio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=51467">Diaocha-lago</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        inter anodo kaj katodo estas elektra tensio de &dash; la&ubreve; la speco de mikroskopo 
        &dash; inter malmultaj <tld/>oj &gcirc;is 3 megavoltoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=134695">Elektrona mikroskopo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_l;&c_a;&c_v;&c_o;&c_l;&c_mol;&c_t;</trd>
  <trd lng="bg">&c_k;&c_i;&c_l;&c_o;&c_v;&c_o;&c_l;&c_t;</trd>
  <trd lng="ca">quilovolt</trd>
  <trd lng="cs">kilovolt</trd>
  <trd lng="de">Kilovolt</trd>
  <trd lng="en">kilovolt</trd>
  <trd lng="es">kilovoltio</trd>
  <trd lng="fa">&f_k;&f_y;&a_l;&a_w;&a_w;&a_l;&a_t;</trd>
  <trd lng="fr">kilovolt</trd>
  <trd lng="hu">kilovolt</trd>
  <trd lng="nl">kilovolt</trd>
  <trd lng="pl">kilowolt</trd>
  <trd lng="ru">&c_k;&c_i;&c_l;&c_o;&c_v;&c_o;&c_l;&c_mol;&c_t;</trd>
  <trd lng="sk">kilovolt</trd>
  <trd lng="uk">&#x43A;&#x456;&#x43B;&#x43E;&#x432;&#x43E;&#x43B;&#x44C;&#x442;</trd>
</drv>
</art>
