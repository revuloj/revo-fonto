<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc><rad>pal</rad>/a
</kap>
<drv mrk="pal.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="pal.0a.vizagxe">
    <uzo tip="klr">(pp viza&gcirc;o)</uzo>
    <dif>
      Neru&gcirc;a, senkolora, malbrile blanka:
      <ekz>
        <tld/>a kiel tolo;
      </ekz>
      <ekz>
        <tld/>a pro timo, kolero;
      </ekz>
      <ekz>
        <tld/>eco de la morto
        (<ref tip="vid" cel="tomb">tomba</ref>).
      </ekz>
    </dif>
  </snc>
  <snc mrk="pal.0a.nebrila">
    <dif>
      Malmulte brila:
      <ekz>
        <tld/>a luno<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a koloro;
      </ekz>
      <ekz>
        sur la <tld/>a &ccirc;ielo la steloj briletis<fnt>B</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hel.0a">hela</ref>.
    </refgrp>
  </snc>
  <snc mrk="pal.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senviveca, sen&ccirc;arma, seninteresa, senforta, senpova:
      <ekz>
        <tld/>aj oficialaj paroloj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>aj jaroj;
      </ekz>
      <ekz>
        doni nur <tld/>an bildon de io;
      </ekz>
      <ekz>
        tiu festo <tld/>igis &ccirc;iujn anta&ubreve;ajn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        lia influo, famo <tld/>i&gcirc;as;
      </ekz>
      <ekz>
        la unua impreso iom post iom <tld/>i&gcirc;as
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="griz.0a">griza</ref>,
      <ref cel="relief.sen0a">senreliefa</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_l;&c_je;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_l;&c_ja;&c_k;&c_l;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">p&agrave;l&middot;lid</trd>
  <trdgrp lng="de">
    <trd>blass</trd>,
    <trd>fahl</trd>
  </trdgrp>
  <trd lng="en">pale</trd>
  <trd lng="es">p&aacute;lido</trd>
  <trdgrp lng="fr">
    <trd>p&acirc;le</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>s&aacute;padt</trd>,
    <trd>halv&aacute;ny</trd>,
    <trd>sz&iacute;ntelen</trd>,
  </trdgrp>  
  <trdgrp lng="nl">
    <trd>bleek</trd>,
    <trd>vaal</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_l;&c_je;&c_d;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="pal.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      I&gcirc;i malpli kolora:
      <ekz>
        &scirc;i <tld/>i&gcirc;is, io kvaza&ubreve; pikis &scirc;ian koron
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 17a, p. 95a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>empal&middot;lidir</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>palidecer</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>p&acirc;lir</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>els&aacute;pad</trd>,
    <trd>elhalv&aacute;nyul</trd>
  </trdgrp>  
</drv>
</art>
