<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pal.xml,v 1.35 2023/10/20 14:25:19 revo Exp $">
<kap>
  <ofc>*</ofc><rad>pal</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pal.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="pal.0a.vizagxe">
    <uzo tip="klr">(pri viza&gcirc;o)</uzo>
    <dif>
      Ne<ref tip="ant" cel="rugx.0a">ru&gcirc;a</ref>, <ref cel="kolor.sen0a">senkolora</ref>, 
      malbrile <ref tip="super" cel="blank.0a">blanka</ref>:
      <ekz>
        li estis <tld/>a de malvarmo kaj tremis per la tuta korpo
        <fnt>
          <bib>Fab1</bib>
          <lok>Malbonkonduta knabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> fari&gcirc;is <tld/>a kiel kreto
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        li fari&gcirc;is <tld/>a kiel kadavro
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a kiel la morto
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a de kolero
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj senkorpaj fantomoj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sepa Kongreso Esperantista en Antwerpen en la 21a de a&ubreve;gusto
          1911</vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="rugx.0vanga">ru&gcirc;vanga</ref>
    <refgrp tip="vid">
      <ref cel="kolor.vaks0a">vakskolora</ref>,
      <ref cel="velk.0a">velka</ref>
    </refgrp>
    <trdgrp lng="ca">
      <trd>l&iacute;vid</trd>,
      <trd>exsang&uuml;e</trd>
    </trdgrp>
    <trd lng="de">bleich</trd>
    <trd lng="ru">&c_b;&c_l;&c_je;&c_d;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="pal.0a.nebrila">
    <dif>
      Malmulte <ref tip="ant" cel="bril.0a">brila</ref>, 
      <ref cel="griz.0a.koloro">grizeta</ref>:
      <ekz>
        la luno <klr>[&#x2026;]</klr> similis al <tld/>a disko sen radioj
        <fnt>
          <bib>Fab4</bib>
          <lok>Anneto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a lumeto de steloj
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;io fandi&gcirc;is en <tld/>a krepusko
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj haroj, disigitaj sur la kapo
        <fnt>
          <bib>IKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi rigardis, kaj jen <tld/>a &ccirc;evalo
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 6:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a hiacinto
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        ravaj herbejoj, kies <tld/>a verda&jcirc;o &gcirc;ojigis la rigardon
        <fnt>
          <bib>KPr</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="hel.0a.blankeca">hela<sncref/></ref>,
       <ref cel="bril.mal0a">malbrila</ref>
    </refgrp>
    <trd lng="ca">clar</trd>
    <trdgrp lng="ru">
      <trd>&c_b;&c_l;&c_je;&c_d;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_b;&c_l;&c_jo;&c_k;&c_l;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="pal.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senviveca, sen&ccirc;arma, <ref cel="intere.sen0ane0a">seninteresa</ref>, 
      senforta, <ref cel="pov.sen0a">senpova</ref>:
      <ekz>
        vi atendas de mi ion <klr>[&#x2026;]</klr> <tld/>an kaj <ref cel="hav.senen0a">senenhava</ref>n, 
        kiel estas ordinare la oficialaj paroloj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Dua Kongreso Esperantista en Gen&egrave;ve en la 28a de a&ubreve;gusto 1906</vrk>
        </fnt>;
      </ekz>
      <ekz>
        vi estas <tld/>aj, mutaj atestantoj
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        li kaj multaj aliaj de tiu sama speco, en kiujn nia <tld/>a tempo estas enamita,
        kaptis nur la tonon de la modo kaj la eksteran brilon
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        mia unua vizito &ccirc;e li <klr>[&#x2026;]</klr> mi havis <tld/>an 
        <klr>(<ref cel="klar.mal0a">malklara</ref>n, <ref cel="preciz.mal0a">malpreciza</ref>n)</klr> 
        memoron <klr>[&#x2026;]</klr> ie en Zubovo
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="griz.0a">griza</ref>,
      <ref cel="relief.sen0a">senreliefa</ref>
    </refgrp>
    <trdgrp lng="ca">
      <trd>dessaborit</trd>,
      <trd>ins&iacute;pid</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_b;&c_l;&c_je;&c_d;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_b;&c_l;&c_jo;&c_k;&c_l;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_l;&c_je;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_l;&c_ja;&c_k;&c_l;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">p&agrave;l&middot;lid</trd>
  <trdgrp lng="cs">
    <trd>mdl&yacute; <klr>(o barv&ecaron;)</klr></trd>,
    <trd>nev&yacute;razn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>blass</trd>,
    <trd>fahl</trd>
  </trdgrp>
  <trd lng="en">pale</trd>
  <trd lng="es">p&aacute;lido</trd>
  <trdgrp lng="fr">
    <trd>p&acirc;le</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>s&aacute;padt</trd>,
    <trd>halv&aacute;ny</trd>,
    <trd>sz&iacute;ntelen</trd>,
  </trdgrp>
  <trd lng="id">pucat</trd>
  <trdgrp lng="ja">
    <trd>&#x9752;&#x3056;&#x3081;&#x305F;  <pr>&#x3042;&#x304A;&#x3056;&#x3081;&#x305F;</pr></trd>,
    <trd>&#x9752;&#x767D;&#x3044;  <pr>&#x3042;&#x304A;&#x3058;&#x308D;&#x3044;</pr></trd>,
    <trd>&#x8840;&#x306E;&#x6C17;&#x306E;&#x306A;&#x3044;  <pr>&#x3061;&#x306E;&#x3051;&#x306E;&#x306A;&#x3044;</pr></trd>,
    <trd>&#x8272;&#x306E;&#x8584;&#x3044;  <pr>&#x3044;&#x308D;&#x306E;&#x3046;&#x3059;&#x3044;</pr></trd>,
    <trd>&#x5149;&#x306E;&#x5F31;&#x3044;  <pr>&#x3072;&#x304B;&#x308A;&#x306E;&#x3088;&#x308F;&#x3044;</pr></trd>,
    <trd>&#x751F;&#x6C17;&#x306E;&#x306A;&#x3044;  <pr>&#x305B;&#x3044;&#x304D;&#x306E;&#x306A;&#x3044;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bleek</trd>,
    <trd>vaal</trd>
  </trdgrp>
  <trd lng="no">blek</trd>
  <trdgrp lng="pl">
    <trd>blady</trd>,
    <trd>siny</trd>
  </trdgrp>
  <trd lng="pt">p&aacute;lido</trd>
  <trdgrp lng="ro">
    <trd>pal</trd>,
    <trd>cianoza</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mdl&yacute; <klr>(o farbe)</klr></trd>,
    <trd>nev&yacute;razn&yacute;</trd>
  </trdgrp>
  <trd lng="sv">blek</trd>
  <trd lng="uk">&c_b;&#x43B;&c_ib;&c_d;&c_i;&c_j;</trd>
</drv>

<drv mrk="pal.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pal.0igi.KOMUNE">
    <dif>
      Igi malpli kolora:
      <ekz>
        virojn mi ser&ccirc;as <klr>[&#x2026;]</klr>, kies sola nomo <klr>[&#x2026;]</klr> la kura&gcirc;ulojn
        timigas kaj la tiranojn <tld/>igas
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        Kaj tiel la melankoli' <tld/>igas /
        Malsane la naturan vang-koloron
        <fnt>
          <aut>W. Shakespeare, trad. L. N. Newell</aut>,
          <vrk>Hamleto, princo de Danujo</vrk>,
          <lok>1964</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="blank.0igi">blankigi</ref>
    </refgrp>
    <trdgrp lng="ca">
      <trd>descolorir</trd>,
      <trd>destenyir</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd><ind>erblassen</ind> lassen</trd>,
      <trd><ind>erbleichen</ind> lassen</trd>
    </trdgrp>
  </snc>
  <snc mrk="pal.0igi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malfortigi, malintensigi:
      <ekz>
        la festoj kaj moroj ne&ubreve;trale-homaj <tld/>igados kaj elpu&scirc;ados la morojn gentajn
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Pri la homaranismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        ies kvalitojn <tld/>igis la fluo de l' jaroj
        <fnt>
          <aut>P. Thorsen</aut>,
          <vrk><url ref="http://www.poulthorsen.dk/finbabile.htm">
          Finbabile?</url></vrk>,
          <lok>Babile kun mi mem. - Beletra, 1996</lok>
        </fnt>.    
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>atenuar</trd>,
      <trd>debilitar</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd><ind>verblassen</ind> lassen</trd>,
      <trd>abschw&auml;chen</trd>,
      <trd>verdr&auml;ngen</trd>
    </trdgrp>
  </snc>
  <refgrp tip="vid">
     <ref cel="stomp.0i">stompi</ref>
  </refgrp>
  <trdgrp lng="cs">
    <trd>b&ecaron;lit</trd>,
    <trd>zp&uring;sobit bledost</trd>
  </trdgrp>
  <trd lng="fr">faire p&acirc;lir</trd>
  <trdgrp lng="ja">
    <trd>&#x9752;&#x3056;&#x3081;&#x3055;&#x305B;&#x308B;  <pr>&#x3042;&#x304A;&#x3056;&#x3081;&#x3055;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x8272;&#x3092;&#x3042;&#x305B;&#x3055;&#x305B;&#x308B;  <pr>&#x3044;&#x308D;&#x3092;&#x3042;&#x305B;&#x3055;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd><ind>bleek</ind> maken</trd>,
    <trd>doen <ind>verbleken</ind></trd>
  </trdgrp>
  <trd lng="no">bleke</trd>
  <trdgrp lng="pl">
    <trd>wywo&lstroke;ywa&cacute; bledni&eogonek;cie</trd>,
    <trd>odbarwia&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>cauza palid</trd>,
    <trd>decolora</trd>
  </trdgrp>
  <trd lng="sk">bieli&tcaron;</trd>
  <trd lng="sv">bleka</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&#x43E;&c_b;&c_i;&c_t;&c_i; &c_b;&#x43B;&c_ib;&c_d;&c_i;&#x43C;</trd>,
    <trd>&c_z;&#x43C;&c_u;&c_sh;&c_u;&c_v;&c_a;&c_t;&c_i; &c_b;&#x43B;&c_ib;&c_d;&#x43D;&c_u;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_t;&#x44C;&#x43C;&c_a;&c_r;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="pal.0igxi">
  <kap><ofc>*</ofc><tld/>i&gcirc;i
  <fnt><bib>F</bib>, <lok>Universala Vortaro, -i&gcirc;'</lok></fnt></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="pal.0igxi.KOL">
    <dif>
      I&gcirc;i malpli kolora:
      <ekz>
        li <tld/>i&gcirc;is de timo kaj poste li ru&gcirc;i&gcirc;is de honto
        <fnt>
          <bib>F</bib>
          <lok>&FE; 39</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>i&gcirc;is, io kvaza&ubreve; pikis &scirc;ian 
        koron
        <fnt><bib>PatrojFiloj</bib>, <lok>&ccirc;apitro 17a, p. 
        95a</lok></fnt>;
      </ekz>
      <ekz>
        li restis firma, ne <tld/>i&gcirc;is, rigardis nur anta&ubreve;en kaj forte tenis la pafilon
        <fnt>
          <bib>Fab1</bib>
          <lok>Persista stana soldato</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la verda rozofrukto pendis ankora&ubreve; nur kelke da <tld/>i&gcirc;intaj folioj
        <fnt>
          <bib>Fab2</bib>
          <lok>Elfo de la rozo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
    <trd>empal&middot;lidir</trd>,
    <trd>esblaimar-se</trd>,
  </trdgrp>
    <trd lng="de">erblassen</trd>
  </snc>
  <snc mrk="pal.0igxi.FIG">
    <dif>
      <ref tip="super" cel="fort.mal0igxi">Malforti&gcirc;i</ref>, malintensi&gcirc;i:
      <ekz>
        lia influo, famo <tld/>i&gcirc;as;
      </ekz>
      <ekz>
        la unua impreso iom post iom <tld/>i&gcirc;as;
      </ekz>
      <ekz>
        sen meditado, scio <tld/>i&gcirc;as
        <fnt>
          <aut>trad. T. T. Anuruddha</aut>,
          <vrk>La Vojo al Nirvano a&ubreve; la Dhammapado, 20:282</vrk>,
          <lok>B&#x1ED3;-T&aacute;t-Csoma-Instituto por Budhologio, 1973</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>decandir-se</trd>,
      <trd>decaure</trd>
    </trdgrp>
    <trd lng="de">verblassen</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_l;&c_ja;&c_d;&c_n;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_l;&c_ja;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>palidecer</trd>
  </trdgrp>
  <trd lng="fr">p&acirc;lir</trd>
  <trdgrp lng="hu">
    <trd>els&aacute;pad</trd>,
    <trd>elhalv&aacute;nyul</trd>
  </trdgrp>
  <trd lng="id"><baz>pucat</baz>memucat</trd>
  <trdgrp lng="ja">
    <trd>&#x9752;&#x3056;&#x3081;&#x308B;  <pr>&#x3042;&#x304A;&#x3056;&#x3081;&#x308B;</pr></trd>,
    <trd>&#x8272;&#x3042;&#x305B;&#x308B;  <pr>&#x3044;&#x308D;&#x3042;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl"><ind>bleek</ind> worden</trd>
  <trd lng="no">blekne</trd>
  <trdgrp lng="pl">
    <trd>blednie&cacute;</trd>,
    <trd>p&lstroke;owie&cacute;</trd>,
    <trd>zbiele&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>decolorare</trd>,
    <trd>albi</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_l;&c_je;&c_d;&c_n;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_b;&c_l;&c_je;&c_d;&c_n;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">blekna</trd>
  <trd lng="uk">&c_b;&#x43B;&c_ib;&c_d;&#x43D;&c_u;&c_t;&c_i;</trd>
</drv>
