<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>testik</rad>/o
</kap>
<drv mrk="testik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      &Ccirc;iu el la du glandoj sekreciantaj la spermon &ccirc;e viraj mambestoj.
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_je;&c_ch;&c_k;&c_a; <klr tip="amb">(&c_a;&c_n;&c_a;&c_t;.)</klr></trd>
  <trdgrp lng="de">
     <trd>Testikel</trd>,
     <trd>Hoden</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>testicle</trd>,
    <trd>testis</trd>
  </trdgrp>
  <trd lng="es">test&iacute;culo</trd> 
  <trdgrp lng="fa">
    <trd>&a_b;&f_y;&a_D;&a_h;</trd>,
    <trd>&a_kh;&a_A;&f_y;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">testicule</trd>
  <trd lng="hu">here <klr tip="amb">(anat.)</klr></trd>
  <trd lng="it">testicolo</trd>
  <trdgrp lng="nl">
    <trd>teelbal</trd>,
    <trd>testikel</trd>
  </trdgrp>
  <trd lng="pl">j&aogonek;dro <klr>(m&eogonek;skie)</klr></trd>
  <trdgrp lng="pt">
    <trd>test&iacute;culo</trd>
  </trdgrp>
  <trd lng="ru">&c_ja;&c_i;&c_ch;&c_k;&c_o; <klr tip="amb">(&c_a;&c_n;&c_a;&c_t;.)</klr></trd>
  <trd lng="kek">ab&#789;aj</trd>
</drv>
</art>
