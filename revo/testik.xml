<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>testik</rad>/o
</kap>
<drv mrk="testik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      &Ccirc;iu el la du glandoj sekreciantaj la spermon &ccirc;e viraj mambestoj.
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_je;&c_ch;&c_k;&c_a; <klr tip="amb">(&c_a;&c_n;&c_a;&c_t;.)</klr></trd>
  <trdgrp lng="de">
     <trd>Testikel</trd>,
     <trd>Hoden</trd>
  </trdgrp>
  <trd lng="en">testis</trd>
  <trd lng="fr">testicule</trd>
  <trdgrp lng="nl">
    <trd>teelbal</trd>,
    <trd>testikel</trd>
  </trdgrp>
  <trd lng="ru">&c_ja;&c_i;&c_ch;&c_k;&c_o; <klr tip="amb">(&c_a;&c_n;&c_a;&c_t;.)</klr></trd>
</drv>
</art>
