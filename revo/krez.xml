<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: krez.xml,v 1.4 2023/04/27 14:27:50 afido Exp $">
    <kap>
      <rad>Krez</rad>/o <fnt><bib>DL</bib></fnt>
    </kap>

<drv mrk="krez.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo><uzo tip="fak">PERS</uzo>
  <snc mrk="krez.0o.regxo">
    <dif>
      Antikva <ref tip="lst" lst="voko:historiaj_personoj" cel="regx.0o">re&gcirc;o</ref>,
      fama pro sia ri&ccirc;eco kaj malavaro, 
      reganta 590 &gcirc;is 541&#x202F;a.K. Lidion, regnon en Malgrandazio 
      situanta en la okcidenta parto de hodia&ubreve;a Turkujo:
      <ekz>
        la unuaj moneroj estis stampitaj de re&gcirc;o <tld/>o de Lidio, kie or-produktejoj
        estis trovitaj
        <fnt>
          <vrk><url ref="&Viki;/Monero">Monero</url></vrk>
        </fnt>;
      </ekz>     
      <ekz>
        la pretigo de la libro de l' vo&ccirc;oj okupos 30 jarojn, kaj por eldoni &gcirc;in vi
        bezonos la ri&ccirc;econ de <tld/>o
        <fnt>
          <bib>DL</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Kr&ouml;sus</trd>
    <trd lng="en">Croesus</trd>
    <trd lng="fr">Cr&eacute;sus</trd>
    <trd lng="grc">&Kappa;&rho;&omikron;&jota_circ;&sigma;&omikron;&sigma_fina;</trd>
    <trd lng="lat">Croesus</trd>
    <trd lng="ru">&c_K;&c_r;&c_jo;&c_z; <klr>(&c_c;&c_a;&c_r;&c_mol; &c_L;&c_i;&c_d;&c_i;&c_i;)</klr></trd>
  </snc>
  <trd lng="be">&c_K;&c_r;&c_o;&c_z; <klr tip="amb">(&c_v;&c_a;&c_l;&c_a;&c_d;&c_a;&c_r; &c_L;&c_ib;&c_d;&c_y;&c_ib;)</klr></trd>
  <trd lng="uk">&#x41A;&c_r;&c_je;&c_z;</trd>
</drv>

  </art>

