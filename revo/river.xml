<drv mrk="river.cxiela0o">
  <kap>&Ccirc;iela <tld lit="R"/>o</kap>
  <snc mrk="river.cxiela0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Orientazia nomo de la
      <ref tip="dif" cel="lakt.0avojo">Lakta Vojo</ref>:
      <ekz>
        levante la kapon, <nom>&Scirc;jangzi</nom> rigardis supren al la
        &Ccirc;iela <tld lit="R"/>o, tiel alta kaj malproksima
        <fnt>
          <aut>Lao She, trad. Wang Chongfang</aut>,
          <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0026/">
          Kamelo &Scirc;jangzi</url></vrk>,
          <lok>1988</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Milchstra&szlig;e</trd>
    <trd lng="en">Milky Way</trd>
    <trdgrp lng="zh">
