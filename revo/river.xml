<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>river</rad>/o
</kap>
<drv mrk="river.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="river.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Natura fluanta, iom forta, akvaro enver&scirc;i&gcirc;anta en
      <tld/>egon a&ubreve; en
      maron:
      <ekz>
        la suno rebrilas en la klara akvo de la <tld/>o
        <fnt><bib>F</bib>, <lok>&FE; 42</lok></fnt>;
      </ekz>
      <ekz>
        <ctl>Kiel akvo de l' <tld/>o</ctl>
        <klr>(romano de Raymond Schwartz)</klr>
        <fnt><aut>Schwartz Raymond</aut>, <vrk>Kiel akvo de l' rivero. -
        La Laguna: J. R&eacute;gulo, 1963</vrk></fnt>.
      </ekz>
    </dif>
    <trd lng="nl">rivier</trd>
  </snc>
  <snc mrk="river.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Granda kvanto da io fluanta a&ubreve; radianta a&ubreve; simila
      al anta&ubreve;eniranta
      flua&jcirc;o:
      <ekz>
        <tld/>oj da larmoj;
      </ekz>
      <ekz>
        suno ver&scirc;is <tld/>ojn da lumo kaj varmego<fnt>B</fnt>;
      </ekz>
      <ekz>
        li englutis <tld/>ojn da konjako<fnt>B</fnt>;
      </ekz>
      <ekz>
        homaj <tld/>oj plenigis la stratojn;
      </ekz>
      <ekz>
        digi la <tld/>on de la reforma agitado<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kanal.0o">kanalo</ref>,
      <ref cel="torent.0o">torento</ref>
    </refgrp>
    <trd lng="nl">stroom</trd>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_k;&c_a;</trd>
  <trd lng="bg">&c_r;&c_je;&c_k;&c_a;</trd>
  <trd lng="ca">riu</trd>
  <trd lng="cs">&rcaron;eka</trd>
  <trd lng="de">Fluss</trd>
  <trd lng="en">river</trd>
  <trd lng="es">r&iacute;o</trd>
  <trdgrp lng="fa">
    <trd>&a_r;&a_w;&a_d;</trd>,
    <trd>&a_r;&a_w;&a_d;&a_kh;&a_A;&a_n;&a_h;</trd>
  </trdgrp>
  <trd lng="hu">foly&oacute;</trd>
  <trd lng="id">sungai</trd>
  <trd lng="fr">rivi&egrave;re</trd>
  <trd lng="pt">rio</trd>
  <trd lng="ru">&c_r;&c_je;&c_k;&c_a;</trd>
  <trd lng="vo">flum</trd>
</drv>

<drv mrk="river.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="river.0ego.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Granda kaj forta <tld/>o, kiu enfluas en la maron.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_k;&c_a;
  <klr>(&c_v;&c_ja;&c_l;&c_ib;&c_k;&c_a;&c_ja;)</klr></trd>
  <trd lng="bg">&c_g;&c_o;&c_l;&c_ja;&c_m;&c_a; &c_r;&c_je;&c_k;&c_a;</trd>
  <trd lng="de">Strom</trd>
  <trd lng="fa">&a_sh;&a_T;</trd>
  <trd lng="fr">fleuve</trd>
  <trd lng="hu">folyam</trd>
  <trd lng="nl">stroom</trd>
  <trd lng="ru"><klr>(&c_b;&c_o;&c_l;&c_mol;&c_sh;&c_a;&c_ja;)</klr>
  &c_r;&c_je;&c_k;&c_a;</trd>
  <trd lng="vo">elflum</trd>
</drv>

<drv mrk="river.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="river.0eto.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Malgranda kaj malforta <tld/>o:
      <ekz>
        <ind><tld/>etoj fluas al <tld/>oj</ind>
        <klr>(per etaj &scirc;paroj oni ri&ccirc;i&gcirc;as)</klr>
        <trd lng="fr">les <ind>petits</ind> ruisseaux font les grandes
        rivi&egrave;res</trd>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_ch;&c_k;&c_a;</trd>
  <trdgrp lng="bg">
    <trd>&c_p;&c_o;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_r;&c_u;&c_ch;&c_je;&c_j;</trd>
  </trdgrp>
  <trd lng="de">Bach</trd>
  <trdgrp lng="fa">
    <trd>&a_j;&a_w;&f_y;</trd>,
    <trd>&a_j;&a_w;&f_y;&a_b;&a_A;&a_r;</trd>,
    <trd>&a_n;&a_h;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">ruisseau</trd>
  <trd lng="hu">patak</trd>
  <trd lng="nl">beek</trd>
  <trd lng="ru">&c_r;&c_je;&c_ch;&c_k;&c_a;</trd>
  <trd lng="vo">bluk</trd>
</drv>

<drv mrk="river.FlavaR0o">
  <kap>Flava <tld lit="R"/>o</kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Granda <tld/>o (4845 km-ojn longa) de la
      <ref tip="malprt" cel="azi.OrientaA0o.GEOG">Orienta Azio</ref>,
      en la Norda &Ccirc;inujo, fluas de Tibeto en la
      <ref cel="mar.FlavaM0o">Flavan Maron</ref>:
      <ekz>
        la Flava <tld lit="R"/>o estas la dua granda <tld/>o de &Ccirc;inio
        <fnt><bib>EncCxi</bib>
        <lok>&ccirc;ap. &leftquot;La Flava Rivero&rightquot;, p. 65a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_H;&c_u;&c_a;&c_n;&c_h;&c_malmol;</trd>,
    <trd>&c_Zh;&c_malmol;&c_l;&c_t;&c_a;&c_t;&c_a; &c_r;&c_je;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Huang He</trd>,
    <trd>Gelber Fluss</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Huang He</trd>,
    <trd>Yellow River</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_h;&a_w;&a_A_madda;&a_n;&f_g; &a_h;&a_w;</trd>,
    <trd>&a_r;&a_w;&a_d; &a_z;&a_r;&a_d;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Huang He</trd>,
    <trd>Fleuve jaune</trd>
  </trdgrp>
  <trd lng="hu">S&aacute;rga-foly&oacute;</trd>
  <trdgrp lng="nl">
    <trd>Huang He</trd>,
    <trd>Gele Rivier</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_H;&c_u;&c_a;&c_n;&c_h;&c_je;</trd>,
    <trd>&c_Zh;&c_jo;&c_l;&c_t;&c_a;&c_ja; &c_r;&c_je;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
