<drv mrk="river.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="river.0ego.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Granda kaj forta <tld/>o:
      <ekz>
        en la mezo de la <tld/>ego
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        por turisto estas strange ekvidi, en la kerno de la urbo, la <tld/>eton <nom>Odet</nom>,
        kiu dufoje dum la tago fari&gcirc;as <tld/>ego
        <fnt>
          <bib>Monato</bib>
          <aut>Josette Ducloyer</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006851.html">Francio:
          Quimper</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la <tld/>ego Luaro
        <fnt>
          <bib>Monato</bib>
          <aut>Lucette &Eacute;chapp&eacute;</aut>
          <vrk><url ref="https://www.monato.be/2013/010930.php">Vizito al Nanto</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_k;&c_a;
  <klr>(&c_v;&c_ja;&c_l;&c_ib;&c_k;&c_a;&c_ja;)</klr></trd>
  <trd lng="bg">&c_g;&c_o;&c_l;&c_ja;&c_m;&c_a; &c_r;&c_je;&c_k;&c_a;</trd>
  <trd lng="cs">veletok</trd>
  <trd lng="de">Strom</trd>
  <trd lng="fa">&a_sh;&a_T;</trd>
  <trd lng="fr">fleuve</trd>
  <trd lng="hu">folyam</trd>
  <trd lng="nl">stroom</trd>
  <trd lng="ru"><klr>(&c_b;&c_o;&c_l;&c_mol;&c_sh;&c_a;&c_ja;)</klr>
  &c_r;&c_je;&c_k;&c_a;</trd>
  <trd lng="sk">ve&lcaron;tok</trd>
  <trd lng="vo">elflum</trd>
</drv>

<drv mrk="river.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="river.0eto.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Malgranda kaj malforta <tld/>o:
      <ekz>
        <ind><tld/>etoj fluas al <tld/>oj</ind>
        <klr>(per etaj &scirc;paroj oni ri&ccirc;i&gcirc;as)</klr>
        <trd lng="fr">les <ind>petits</ind> ruisseaux font les grandes
        rivi&egrave;res</trd>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        li havis anta&ubreve; si la tutan <tld/>eton kun la klara, fre&scirc;a akvo
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        kviete serpentumantaj <tld/>etoj kaj kanaloj
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i iris al la <tld/>eto, kiu fluis tra la &gcirc;ardeno, kaj ordonis kapti sitelon da
        gugonoj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fabelo pri iu, kiu migris por ekkoni timon</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_ch;&c_k;&c_a;</trd>
  <trdgrp lng="bg">
    <trd>&c_p;&c_o;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_r;&c_u;&c_ch;&c_je;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>potok</trd>,
    <trd>&rcaron;&iacute;&ccaron;ka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bach</trd>,
    <trd>Fl&uuml;sschen</trd>
  </trdgrp>
  <trd lng="es">riachuelo</trd>
  <trdgrp lng="fa">
    <trd>&a_j;&a_w;&f_y;</trd>,
    <trd>&a_j;&a_w;&f_y;&a_b;&a_A;&a_r;</trd>,
    <trd>&a_n;&a_h;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">ruisseau</trd>
  <trd lng="hu">patak</trd>
  <trd lng="nl">beek</trd>
  <trd lng="ru">&c_r;&c_je;&c_ch;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>potok</trd>,
    <trd>rie&ccaron;ka</trd>
  </trdgrp>
  <trd lng="vo">bluk</trd>
</drv>

<drv mrk="river.Blua_0o">
  <kap>
    Blua <tld lit="R"/>o
  </kap>
  <snc mrk="river.Blua_0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="dif" cel="jangzi.0o">Jangzio</ref>:
      <ekz>
        Jangzio, anka&ubreve; nomata Blua <tld lit="R"/>o, estas la plej longa <tld/>o en Azio
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Jangzio">Jangzio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_Ja;&c_n;&c_d;&c_z;&c_y;</trd>,
      <trd>&c_B;&c_l;&c_a;&c_k;&c_ib;&c_t;&c_n;&c_a;&c_ja; 
  &c_r;&c_a;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="ca">Iang-Ts&eacute;</trd>
    <trdgrp lng="de">
      <trd>Jangtsekiang</trd>,
      <trd>Jangtse-Fluss</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>Yangtze Kiang</trd>,
      <trd>Chang Jiang</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>Chang jiang</trd>,
      <trd>Yang-tseu-kiang</trd>,
      <trd>Yangzi jiang</trd>
    </trdgrp>
    <trd lng="id"><baz>sungai</baz>Sungai Yangtze</trd>
    <trd lng="nl">Jangtsekiang</trd>
    <trdgrp lng="ru">
      <trd>&c_Ja;&c_n;&c_c;&c_z;&c_y;</trd>,
      <trd>&c_G;&c_o;&c_l;&c_u;&c_b;&c_a;&c_ja; &c_r;&c_je;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
<trd lng="zh">&#x957F;&#x6C5F;</trd>
</drv>


<drv mrk="river.cxiela0o">
  <kap>&Ccirc;iela <tld lit="R"/>o</kap>
  <snc mrk="river.cxiela0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Orientazia nomo de la
      <ref tip="dif" cel="lakt.0avojo">Lakta Vojo</ref>:
      <ekz>
        levante la kapon, <nom>&Scirc;jangzi</nom> rigardis supren al la
        &Ccirc;iela <tld lit="R"/>o, tiel alta kaj malproksima
        <fnt>
          <aut>Lao She, trad. Wang Chongfang</aut>,
          <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0026/">
          Kamelo &Scirc;jangzi</url></vrk>,
          <lok>1988</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Milchstra&szlig;e</trd>
  </snc>
  <trd lng="fr">Rivi&egrave;re c&eacute;leste<klr> (voie lact&eacute;e)</klr></trd>
</drv>

<drv mrk="river.FlavaR0o">
  <kap>Flava <tld lit="R"/>o</kap>
  <snc mrk="river.FlavaR0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Granda <ref tip="lst" lst="voko:riveroj" cel="river.0o.GEOG">rivero</ref> (4845&#x202F;km-ojn longa)
      de la
      <ref tip="malprt" cel="azi.OrientaA0o.GEOG">Orienta Azio</ref>,
      en la Norda &Ccirc;inujo, fluas de Tibeto en la
      <ref cel="mar.FlavaM0o">Flavan Maron</ref>:
      <ekz>
        la Flava <tld lit="R"/>o estas la dua granda <tld/>o de &Ccirc;inio
        <fnt><bib>EncCxi</bib>
        <lok>&ccirc;ap. &leftquot;La Flava Rivero&rightquot;, p. 65a</lok></fnt>;
      </ekz>
      <ekz>
        la ruino atestas, ke anka&ubreve; la baseno de Jangzio estas fonto de la &ccirc;ina
        civilizacio, <klr>[&#x2026;]</klr> anta&ubreve; 5000 &gcirc;is 3000 jaroj tie estis &ccirc;efurbo de la
        regno kaj kultura centro interkomunika<klr>[n]</klr>ta kun la meza kaj
        malsupra baseno de la Flava <tld lit="R"/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="https://www.monato.be/2009/009901.php">Enigmoj de Sanxingdui</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_H;&c_u;&c_a;&c_n;&c_h;&c_e;</trd>,
    <trd>&c_Zh;&c_o;&c_w;&c_t;&c_a;&c_ja; &c_r;&c_a;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_H;&c_u;&c_a;&c_n;&c_h;&c_malmol;</trd>,
    <trd>&c_Zh;&c_malmol;&c_l;&c_t;&c_a;&c_t;&c_a; &c_r;&c_je;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Huang He</trd>,
    <trd>Gelber Fluss</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Huang He</trd>,
    <trd>Yellow River</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_h;&a_w;&a_A_madda;&a_n;&f_g; &a_h;&a_w;</trd>,
    <trd>&a_r;&a_w;&a_d; &a_z;&a_r;&a_d;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Huang He</trd>,
    <trd>Fleuve jaune</trd>
  </trdgrp>
  <trd lng="hu">S&aacute;rga-foly&oacute;</trd>
  <trd lng="id"><baz>sungai</baz>Sungai Kuning</trd>
  <trdgrp lng="nl">
    <trd>Huang He</trd>,
    <trd>Gele Rivier</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_H;&c_u;&c_a;&c_n;&c_h;&c_je;</trd>,
    <trd>&c_Zh;&c_jo;&c_l;&c_t;&c_a;&c_ja; &c_r;&c_je;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#x9EC4;&#x6CB3;</trd>
</drv>

<drv mrk="river.Perlo0o">
  <kap>
    Perlo<tld/>o
  </kap>
  <snc mrk="river.Perlo0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" lst="voko:riveroj" cel="river.0o.GEOG">Rivero</ref> 
      en la sudo de <ref tip="malprt" cel="cxin.CX0ujo">&Ccirc;inujo</ref>
      pasanta <ref cel="kanton2.0o">Kantono</ref>n kaj fluanta inter
      <ref cel="maka.0o">Makao</ref> kaj <ref cel="honkong.0o">Honkongo</ref>
      en la <ref cel="mar.SudaCXinaM0o">Sud&ccirc;inan Maron</ref>:
      <ekz>
        Makao <klr>[&#x2026;]</klr> situas &ccirc;e la delto de la Perlo<tld/>o, limante kun Kantonio
        norde kaj kun la Sud&ccirc;ina Maro sude
        <fnt>
          <vrk><url ref="https://wikihihi.com/post/eo/Makao">Makao</url></vrk>
          <lok>wikihihi.com</lok>
        </fnt>;
      </ekz>
      <ekz>
        55 mil homoj forlasis la urbon per trajnoj <klr>[&#x2026;]</klr>, &ccirc;irka&ubreve; 40&#x202F;% de ili
        iras al la Delta Regiono de la Perlo<tld/>o
        <fnt>
          <vrk><url ref="https://eo.wikinews.org/wiki/Uhano_malfermi%C4%9Dis_post_la_kvaranteno">Uhano
          malfermi&gcirc;is post la kvaranteno</url></vrk>
          <lok>Vikinova&jcirc;oj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Perlfluss</trd>
  <trd lng="en">Pearl River</trd>
