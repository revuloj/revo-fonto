<drv mrk="river.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="river.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Natura fluanta, iom abunda, akvomaso, enver&scirc;i&gcirc;anta, se ne seki&gcirc;ante, en
      maron, lagon a&ubreve; alian riveron:
      <ekz>
        la suno rebrilas en la klara akvo de la <tld/>o
        <fnt><bib>F</bib>, <lok>&FE; 42</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>o eliras el <nom>Eden</nom>, por akvoprovizi la &gcirc;ardenon, kaj de tie &gcirc;i dividi&gcirc;as kaj
        fari&gcirc;as kvar &ccirc;efpartoj
        <klr>(vd <ref cel="euxfrat.E0o">E&ubreve;frato</ref>, <ref cel="tigris.T0o">Tigriso</ref>)</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 2:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu &ccirc;i <tld/>o havas ducent na&ubreve;dek kvar kilometrojn da longo
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o mu&gcirc;is, &scirc;veligita de la printempaj pluvoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        saliko staras super la <tld/>o
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        atendi sur tero, &gcirc;is seki&gcirc;os la <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        el malgrandaj akveroj fari&gcirc;as grandaj <tld/>oj
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la man&gcirc;ota fi&scirc;o estas ankora&ubreve; en la <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        li trafis el sub pluvo en <tld/>on
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ctl>Kiel akvo de l' <tld/>o</ctl>
        <klr>(romano de <nom>Raymond Schwartz</nom>)</klr>
        <fnt><aut>Schwartz Raymond</aut>, <vrk>Kiel akvo de l' rivero. -
        La Laguna: J. R&eacute;gulo, 1963</vrk></fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
       <ref cel="font.0o.loko">fonto<sncref/></ref>,
       <ref cel="flu.0ejo">fluejo</ref>,
       <ref cel="meandr.0o.GEOG">meandro<sncref/></ref>,
       <ref cel="katarakt.0o">katarakto</ref>,
       <ref cel="fal.akvo0o">akvofalo</ref>,
       <ref cel="flu.kun0ejo">kunfluejo</ref>,
       <ref cel="flu.en0ejo">enfluejo</ref>
    </refgrp>
    <trd lng="nl">rivier</trd>
  </snc>
  <snc mrk="river.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Granda kvanto da io fluanta a&ubreve; radianta a&ubreve; simila
      al anta&ubreve;eniranta flua&jcirc;o:
      <ekz>
        ver&scirc;ante <tld/>on da larmoj
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la pluvo faladis per <tld/>oj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi na&gcirc;is tra ia interna <tld/>o de la oceano
        <klr>(kp <ref cel="flu.0o">fluo</ref>)</klr>
        <fnt>
          <bib>FdO</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de la lunaj radioj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        etendinte la brakojn mi ver&scirc;us <tld/>ojn da radioj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> trinkis <tld/>ojn da biero
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kanal.0o">kanalo</ref>,
      <ref cel="torent.0o">torento</ref>
    </refgrp>
    <trd lng="nl">stroom</trd>
  </snc>
