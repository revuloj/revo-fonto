  <trd lng="pl">Huang He  <klr>(&Zdot;&oacute;&lstroke;ta Rzeka)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_H;&c_u;&c_a;&c_n;&c_h;&c_je;</trd>,
