<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kilovat</rad>/o
</kap>
<drv mrk="kilova.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <fnt><bib>PV</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de 
      <ref cel="pov.0o.FIZ">povo</ref>, egala al
      mil <ref tip="prt" cel="vat1.v0o">vatoj</ref>; simb. kW.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_l;&c_a;&c_v;&c_a;&c_t;</trd>
  <trd lng="de">Kilowatt</trd>
  <trd lng="en">kilowatt</trd>
  <trd lng="fr">kilowatt</trd>
  <trd lng="hu">kilowatt</trd>
  <trd lng="nl">kilowatt</trd>
  <trd lng="ru">&c_k;&c_i;&c_l;&c_o;&c_v;&c_a;&c_t;&c_t;</trd>
</drv>

<drv mrk="kilova.0horo">
  <kap><tld/>horo</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Komunuza <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de
      <ref cel="energi.0o.FIZ">energio</ref>>, egala al
      mil <ref tip="prt" cel="vat1.v0horo">vathoroj</ref>; simb. kWh.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_l;&c_a;&c_v;&c_a;&c_t;-&c_g;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_a;</trd>
  <trd lng="de">Kilowattstunde</trd>
  <trd lng="en">kilowatt-hour</trd>
  <trd lng="fr"><ind>kilowatt</ind>-heure</trd>
  <trd lng="hu">kilowatt&oacute;ra</trd>
  <trd lng="nl">kilowattuur</trd>
  <trd lng="ru">&c_k;&c_i;&c_l;&c_o;&c_v;&c_a;&c_t;&c_t;-&c_ch;&c_a;&c_s;</trd>
</drv>
</art>
