  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_t;&c_o;&c_v;&c_y; &c_p;&c_r;&c_y;&c_n;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_k;&c_r;&c_y;&c_t;&c_y;</trd>
  </trdgrp>
