<drv mrk="akcept.0ema">
  <kap>
    <tld/>ema
  </kap>
  <snc>
    <dif>
      Inklina, preta <tld/>i ion a&ubreve; iun:
      <ekz>
        afabla, komfortema, <tld/>ema
        <fnt>
          <bib>KKr</bib>
        </fnt>;
      </ekz><ekz>
        lar&gcirc;kora <tld/>emo,
        <fnt>
          <bib>BonaLingvo</bib>
        </fnt>;
      </ekz><ekz>
        koro ankora&ubreve; fre&scirc;a kaj juna kaj ankora&ubreve; <tld/>ema por feli&ccirc;o
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz><ekz>
        en &scirc;ia rigardo <klr>[&#8230;]</klr> trovi&gcirc;is nur <tld/>ema vivo&gcirc;ojo
        <fnt>
          <bib>VRA</bib>
        </fnt>;
      </ekz><ekz>
        la kuracistoj estis <tld/>emaj al novaj ideoj
        <fnt>
          <bib>Sol</bib>
        </fnt>;
      </ekz><ekz>
        se li konservos <klr>[&#8230;]</klr> sian <tld/>emon por intelektula vivo, &ndash; &gcirc;uste tio
        estas inteligenteco&#8221;
        <fnt>
          <bib>KFl</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dispon.0ita">disponita</ref>,
      <ref cel="ferm.mal0ita.PSI">malfermita<sncref/></ref>
