    <trd>&#21542;&#27770;&#12377;&#12427;  [<ind>&#12402;&#12369;&#12388;&#12377;&#12427;</ind>]</trd>
  </trdgrp>
  <trd lng="sk">odmietn&uacute;&tcaron;</trd>
