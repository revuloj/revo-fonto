<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: naning.xml,v 1.4 2022/11/30 18:35:10 afido Exp $">
    <kap>
      <rad>Naning</rad>/o
    </kap>

<drv mrk="naning.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Granda <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      en la sudo de <ref tip="super" cel="cxin.CX0ujo">&Ccirc;inujo</ref>, nordoriente de Hanojo:
      <ekz>
        en &ccirc;irka&ubreve;a&jcirc;o de <tld/>o oni kultivas sukerkanojn
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Naningo">Naningo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la lasta sin&jcirc;eto &ndash; veturo al <tld/>o, kaj fine al Hanojo
        <fnt>
          <vrk><url ref="&Sezonoj;2012/08/215uk-2/">Karavane al UK</url></vrk>
          <lok> La Ondo de Esperanto, afi&scirc;ita en 15 A&ubreve;gusto 2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_N;&c_a;&c_n;&c_mol;&c_n;&c_ib;&c_n;</trd>
    <trd lng="de">Nanning</trd>
    <trd lng="en">Nanning</trd>
    <trd lng="fr">Nanning</trd>
    <trd lng="pl">Nanning</trd>
    <trd lng="ro">Nanning</trd>
    <trdgrp lng="zh">
      <trd>&#21335;&#23527;</trd>,
      <trd>&#21335;&#23425;</trd>
    </trdgrp>
  </snc>
</drv>

  </art>

