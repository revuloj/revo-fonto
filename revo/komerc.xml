    <trd>obchodn&iacute;</trd>,
    <trd>trhov&yacute;</trd>,
    <trd>tr&zcaron;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Handels-</trd>,
    <trd>kommerziell</trd>
  </trdgrp>
  <trd lng="en">commercial</trd>
  <trd lng="es">comercial</trd>
  <trdgrp lng="fr">
    <trd>commercial</trd>
  </trdgrp>
  <trd lng="hu">kereskedelmi</trd>
  <trd lng="id">komersial</trd>
