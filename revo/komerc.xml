    <uzo tip="fak">EKON</uzo>
    <dif>
      Profesia klopodo por la inter&scirc;an&gcirc;o de varoj:
      <ekz>
        <ind>pogranda <tld/>o</ind>;
        <trdgrp lng="hu">
          <trd>nagykereskedelem</trd>,
          <trd>nagybani <ind>keresked&eacute;s</ind></trd>
        </trdgrp>
        <trd lng="pl">handel hurtowy</trd>
      </ekz>
      <ekz>
        <ind>pomalgranda <tld/>o</ind>;
        <trd lng="hu">kiskereskedelem</trd>
        <trd lng="pl">handel detaliczny</trd>
      </ekz>      
      <ekz>
        <ind>ne ekzistas en <tld/>o amikeco nek &scirc;erco</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="fr">en <ind>affaire</ind>s, on ne fait pas de
        sentiments</trd>
