<drv mrk="komerc.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <tezrad fak="EKON"/>
  <snc mrk="komerc.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Profesie <ref tip="super" cel="sxangx.inter0i">inter&scirc;an&gcirc;i</ref> varojn, produkta&jcirc;ojn,
      celante monprofiton:
      <ekz>
        &ccirc;iuj princoj de <nom>Kedar</nom> <tld/>is kun vi, donante al vi &scirc;afidojn, &scirc;afojn, kaj
        kaprojn kontra&ubreve; viaj <tld/>a&jcirc;oj
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 27:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        homoj pli spritaj <tld/>is je kuketoj, kvaso, malkostaj orelbukoj, ringoj,
        argilaj fajfiletoj, ludiloj, koloraj &scirc;tofoj, galanterio
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la sklavvendistoj siatempe ne rigardis nigrajn afrikanojn membroj de la homa
        raso, tial ili senhonte ilin kaptadis, kaj <tld/>is per ili
        <fnt>
          <bib>ChV</bib>
          <lok>7</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu firmao <klr>[&#8230;]</klr> komercas landskale
        <fnt>
          <bib>KrM</bib>
          <lok>11. Petro Rifuzas, Pa&ubreve;lo Ekuzas</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne &scirc;atus reiri al dungiteco post mia longa klopodado <tld/>i proprakonte
        <fnt>
          <bib>KrM</bib>
          <lok>39. Ni Forton Pli Fre&scirc;an Akiras.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>kiu volas <tld/>i, tiu sa&gcirc;on bezonas</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="hu">az &uuml;zlethez &eacute;sz kell</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="entrep.0i">entrepreni</ref>,
      <ref cel="negoc.0i">negoci</ref>,
      <ref cel="vend.0i">vendi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_n;&c_d;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">comerciar</trd>
  <trd lng="cs">obchodovat</trd>
  <trd lng="de">handeln</trd>
  <trd lng="en">trade</trd>
  <trd lng="es">comerciar</trd>
  <trdgrp lng="fr">
    <trd>commercer</trd>,
    <trd>faire du <ind>commerce</ind></trd>,
    <trd>faire du <ind>n&eacute;goce</ind></trd>
  </trdgrp>
  <trd lng="hu">kereskedik</trd>
  <trdgrp lng="id">
    <trd><baz>dagang</baz>berdagang</trd>,
    <trd><baz>jual</baz>berjual beli</trd>,
    <trd><baz>niaga</baz>berniaga</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#21830;&#22770;&#12434;&#12377;&#12427;  <pr>&#12375;&#12423;&#12358;&#12400;&#12356;&#12434;&#12377;&#12427;</pr></trd>,
    <trd>&#21830;&#21462;&#24341;&#12434;&#12377;&#12427;  <pr>&#12375;&#12423;&#12358;&#12392;&#12426;&#12402;&#12365;&#12434;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">handel drijven</trd>
  <trd lng="pl">handlowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>comerciar</trd>,
    <trd>negociar</trd>,
    <trd>mercadejar</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>comer&#539;</trd>,
    <trd>comercializa</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_o;&c_r;&c_g;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">obchodova&tcaron;</trd>
  <trdgrp lng="zh">
