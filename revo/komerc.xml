<drv mrk="komerc.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <tezrad fak="EKON"/>
  <snc mrk="komerc.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Profesie <ref tip="super" cel="sxangx.inter0i">inter&scirc;an&gcirc;i</ref> varojn, produkta&jcirc;ojn,
      celante monprofiton:
      <ekz>
        &ccirc;iuj princoj de <nom>Kedar</nom> <tld/>is kun vi, donante al vi &scirc;afidojn, &scirc;afojn, kaj
        kaprojn kontra&ubreve; viaj <tld/>a&jcirc;oj
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 27:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        homoj pli spritaj <tld/>is je kuketoj, kvaso, malkostaj orelbukoj, ringoj,
        argilaj fajfiletoj, ludiloj, koloraj &scirc;tofoj, galanterio
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la sklavvendistoj siatempe ne rigardis nigrajn afrikanojn membroj de la homa
        raso, tial ili senhonte ilin kaptadis, kaj <tld/>is per ili
        <fnt>
          <bib>ChV</bib>
          <lok>7</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu firmao <klr>[&#8230;]</klr> komercas landskale
        <fnt>
          <bib>KrM</bib>
          <lok>11. Petro Rifuzas, Pa&ubreve;lo Ekuzas</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne &scirc;atus reiri al dungiteco post mia longa klopodado <tld/>i proprakonte
        <fnt>
          <bib>KrM</bib>
          <lok>39. Ni Forton Pli Fre&scirc;an Akiras.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>kiu volas <tld/>i, tiu sa&gcirc;on bezonas</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="hu">az &uuml;zlethez &eacute;sz kell</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="entrep.0i">entrepreni</ref>,
      <ref cel="negoc.0i">negoci</ref>,
      <ref cel="vend.0i">vendi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_n;&c_d;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">obchodovat</trd>
  <trd lng="de">handeln</trd>
  <trd lng="en">trade</trd>
  <trd lng="es">comerciar</trd>
  <trdgrp lng="fr">
    <trd>commercer</trd>,
    <trd>faire du <ind>commerce</ind></trd>,
    <trd>faire du <ind>n&eacute;goce</ind></trd>
  </trdgrp>
  <trd lng="hu">kereskedik</trd>
  <trdgrp lng="id">
    <trd><baz>dagang</baz>berdagang</trd>,
    <trd><baz>jual</baz>berjual beli</trd>,
    <trd><baz>niaga</baz>berniaga</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#21830;&#x58F2;&#12434;&#12377;&#x308B;  <pr>&#12375;&#12423;&#12358;&#12400;&#12356;&#12434;&#12377;&#x308B;</pr></trd>,
    <trd>&#21830;&#x53D6;&#x5F15;&#12434;&#12377;&#x308B;  <pr>&#12375;&#12423;&#12358;&#12392;&#x308A;&#12402;&#x304D;&#12434;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">handel drijven</trd>
  <trd lng="pl">handlowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>comerciar</trd>,
    <trd>negociar</trd>,
    <trd>mercadejar</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>comer&#x21B;</trd>,
    <trd>comercializa</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_o;&c_r;&c_g;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">obchodova&tcaron;</trd>
  <trdgrp lng="zh">
    <trd>&#x71DF;&#x696D; <pr>y&#xED;ngy&#xE8;</pr></trd>,
    <trd>&#25443;&#x9F13; <pr>d&#x1CE;ogu</pr></trd>,
    <trd>&#x8CBF;&#26131; <pr>m&#xE0;oy&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="komerc.0o">
  <kap><tld/>o</kap>
  <snc mrk="komerc.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Profesia klopodo por la inter&scirc;an&gcirc;o de varoj:
      <ekz>
        <tld/>o de agrikulturaj produkta&jcirc;oj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        floranta <tld/>o
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        amasa ma&scirc;ina produktado por la <ref cel="merkat.0o">merkato</ref>
        de <klr>[&#8230;]</klr> granda internacia <tld/>o
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        sescent sesdek ses kikaroj da oro <klr>[&#8230;]</klr> venis de <klr>[&#8230;]</klr> la
        <tld/>o de negocistoj
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 10:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o en Kopenhago trovi&gcirc;is tiam en la manoj de la ri&ccirc;aj <tld/>istoj de
        Bremo kaj Lubeko, ili mem ne venadis, sed ili sendadis siajn komizojn
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam li post maturi&gcirc;inta pripenso a&ccirc;etis feruzinajn akciojn <klr>[&#8230;]</klr> li
        estis montfirme konvinkita, ke tio estas la plej bona <tld/>o, kiun homo
        povas fari
        <fnt>
          <bib>HsT</bib>
          <lok>Disaj vojoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        miaj fratoj devos vendi nian <tld/>on al la firmo <nom>Kensford</nom>
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 9</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>pogranda <tld/>o</ind>;
        <trdgrp lng="hu">
          <trd>nagykereskedelem</trd>,
          <trd>nagybani <ind>keresked&eacute;s</ind></trd>
        </trdgrp>
        <trd lng="pl">handel hurtowy</trd>
      </ekz>
      <ekz>
        <ind>pomalgranda <tld/>o</ind>;
        <trd lng="hu">kiskereskedelem</trd>
        <trd lng="pl">handel detaliczny</trd>
      </ekz>
      <ekz>
        <ind>ne ekzistas en <tld/>o amikeco nek &scirc;erco</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="fr">en <ind>affaire</ind>s, on ne fait pas de
        sentiments</trd>
        <trd lng="hu">az &uuml;zlet, az &uuml;zlet</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="afer.0o.komerco">afero<sncref/></ref>,
      <ref cel="entrep.0o">entrepreno</ref>,
      <ref cel="negoc.0o">negoco</ref>
