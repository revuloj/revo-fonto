<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>komerc</rad>/i
</kap>
<drv mrk="komerc.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">KOME</uzo>
  <snc>
    <dif>
      Profesie inter&scirc;an&gcirc;i varojn, produkta&jcirc;ojn, celante monprofiton:
      <ekz>
        kiu volas <tld/>i, tiu sa&gcirc;on bezonas<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        Anglujo <tld/>as kun la tuta mondo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="negoc.0i">negoci</ref>
    </refgrp>
  </snc>
  <trd lng="de">handeln</trd>
  <trd lng="en">trade</trd>
  <trdgrp lng="fr">
    <trd>commercer</trd>,
    <trd>faire du <ind>n&eacute;goce</ind></trd>
  </trdgrp>
  <trd lng="nl">handel drijven</trd>
  <trd lng="ru">&c_t;&c_o;&c_r;&c_g;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="komerc.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Profesia klopodo por la inter&scirc;an&gcirc;o de varoj:
      <ekz>
        pogranda, pomalgranda <tld/>o;
      </ekz>
      <ekz>
        ne ekzistas en <tld/>o amikeco nek &scirc;erco<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="negoc.0o">negoco</ref>,
      <ref cel="afer.0o">afero 4</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Handel</trd>
  <trdgrp lng="fr">
    <trd>commerce<klr> (activit&eacute; commerciale)</klr></trd>,
    <trd>n&eacute;goce</trd>
  </trdgrp>
  <trd lng="nl">handel</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_m;&c_je;&c_r;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_t;&c_o;&c_r;&c_g;&c_o;&c_v;&c_l;&c_ja;</trd>,
    <trd>&c_b;&c_i;&c_z;&c_n;&c_je;&c_s;</trd>
  </trdgrp>
</drv>

<drv mrk="komerc.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a valoro de objekto;
      </ekz>
      <ekz>
        <tld/>aj kontraktoj;
      </ekz>
      <ekz>
        <tld/>a &ccirc;ambro
        <klr>(komitato da <tld/>istoj elektitaj por studi la
        <tld/>ajn interesojn)</klr>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="negoc.0a">negoca</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Handels-</trd>,
     <trd>kommerziell</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>commercial</trd>
  </trdgrp>
  <trd lng="nl">handels-</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_m;&c_je;&c_r;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_t;&c_o;&c_r;&c_g;&c_o;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="komerc.0ajxo">
  <kap><ofc>*</ofc><tld/>a&jcirc;o</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">KOME</uzo>
  <snc>
    <dif>
      A&jcirc;o a&ubreve; servo akirebla kontra&ubreve; pago.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="var.0o">varo</ref>
  </refgrp>
  <trdgrp lng="de">
     <trd>Ware</trd>,
     <trd>Handelsgut</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>marchandise</trd>
  </trdgrp>
  <trd lng="nl">handelswaar</trd>
  <trd lng="ru">&c_t;&c_o;&c_v;&c_a;&c_r;</trd>
</drv>

<drv mrk="komerc.0ejo">
  <kap><tld/>ejo</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">KOME</uzo>
  <snc>
    <dif>
      Loko, kie oni inter&scirc;an&gcirc;as <tld/>a&jcirc;ojn.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Markt</trd>,
     <trd>Marktplatz</trd>
  </trdgrp>
  <trd lng="nl">markt</trd>
</drv>

<drv mrk="komerc.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">KOME</uzo>
  <snc>
    <dif>
      Homo, kiu pofesie komercadas.
    </dif>
    <refgrp tip="sin">
      <ref cel="negoc.0isto">negocisto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>H&auml;ndler</trd>,
     <trd>Gesch&auml;ftsmann</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>commer&ccedil;ant<klr> (toute activit&eacute; commerciale)</klr></trd>,
    <trd>n&eacute;gociant</trd>
  </trdgrp>
</drv>

<drv mrk="komerc.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Forvendi:
      <ekz>
        eble mi devos el la vestoj ion for<tld/>i<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>verkaufen</trd>,
     <trd>losschlagen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;couler<klr> (une marchandise)</klr></trd>
  </trdgrp>
  <trd lng="nl">verkopen</trd>
</drv>
</art>
