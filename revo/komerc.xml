<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>komerc</rad>/i
</kap>
<drv mrk="komerc.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Profesie inter&scirc;an&gcirc;i varojn, produkta&jcirc;ojn, 
      celante monprofiton:
      <ekz>
        kiu volas <tld/>i, tiu sa&gcirc;on bezonas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        Anglujo <tld/>as kun la tuta mondo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="negoc.0i">negoci</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_n;&c_d;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">handeln</trd>
  <trd lng="en">trade</trd>
  <trd lng="es">comerciar</trd>
  <trdgrp lng="fr">
    <trd>commercer</trd>,
    <trd>faire du <ind>commerce</ind></trd>,
    <trd>faire du <ind>n&eacute;goce</ind></trd>
  </trdgrp>
  <trd lng="nl">handel drijven</trd>
  <trd lng="ru">&c_t;&c_o;&c_r;&c_g;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="komerc.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Profesia klopodo por la inter&scirc;an&gcirc;o de varoj:
      <ekz>
        pogranda, pomalgranda <tld/>o;
      </ekz>
      <ekz>
        ne ekzistas en <tld/>o amikeco nek &scirc;erco
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="afer.0o.komerco">afero</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_n;&c_d;&c_a;&c_l;&c_mol;</trd>,
    <trd>&c_b;&c_i;&c_z;&c_n;&c_e;&c_s;</trd>,
    <trd>&c_k;&c_a;&c_m;&c_e;&c_r;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_g;&c_je;&c_sh;&c_e;&c_f;&c_t;</trd>
  </trdgrp>
  <trd lng="de">Handel</trd>
  <trd lng="es">comercio</trd>
  <trdgrp lng="fr">
    <trd>commerce<klr tip="ind"> (activit&eacute; commerciale)</klr></trd>,
    <trd>n&eacute;goce</trd>
  </trdgrp>
  <trd lng="nl">handel</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_m;&c_je;&c_r;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_t;&c_o;&c_r;&c_g;&c_o;&c_v;&c_l;&c_ja;</trd>,
    <trd>&c_b;&c_i;&c_z;&c_n;&c_je;&c_s;</trd>
  </trdgrp>
</drv>

<drv mrk="komerc.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a valoro de objekto;
      </ekz>
      <ekz>
        <tld/>aj kontraktoj;
      </ekz>
      <ekz>
        <tld/>a &ccirc;ambro
        <klr>(komitato da <tld/>istoj elektitaj por studi la
        <tld/>ajn interesojn)</klr>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_m;&c_e;&c_r;&c_c;&c_y;&c_j;&c_n;&c_y;</trd>,
    <trd>&c_g;&c_a;&c_n;&c_d;&c_l;&c_jo;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Handels-</trd>,
    <trd>kommerziell</trd>
  </trdgrp>
  <trd lng="es">comercial</trd>
  <trdgrp lng="fr">
    <trd>commercial</trd>
  </trdgrp>
  <trd lng="nl">handels-</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_m;&c_je;&c_r;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_t;&c_o;&c_r;&c_g;&c_o;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="komerc.0ajxo">
  <kap><ofc>*</ofc><tld/>a&jcirc;o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      A&jcirc;o a&ubreve; servo akirebla kontra&ubreve; pago.
    </dif>
    <refgrp tip="sin">
      <ref cel="var.0o">varo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_v;&c_a;&c_r;</trd>
  <trdgrp lng="de">
    <trd>Ware</trd>,
    <trd>Handelsgut</trd>
  </trdgrp>
  <trd lng="es">mercanc&iacute;a</trd>
  <trdgrp lng="fr">
    <trd>marchandise</trd>
  </trdgrp>
  <trd lng="nl">handelswaar</trd>
  <trd lng="ru">&c_t;&c_o;&c_v;&c_a;&c_r;</trd>
</drv>

<drv mrk="komerc.0ejo">
  <kap><tld/>ejo</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Loko, kie oni <tld/>as.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_y;&c_n;&c_a;&c_k;</trd>
  <trdgrp lng="de">
    <trd>Markt</trd>,
    <trd>Marktplatz</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>commerce<klr tip="ind"> (magasin)</klr></trd>,
    <trd>magasin</trd>
  </trdgrp>
  <trd lng="nl">markt</trd>
</drv>

<drv mrk="komerc.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Homo, kiu pofesie komercadas:
      <ekz>
        regularo uzata en la tuta mondo de <tld/>istoj, juristoj,
        transportistoj kaj asekuristoj
        <fnt><aut>-</aut>: <vrk>Inkotermoj 2000</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 3a, p. 29a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_n;&c_d;&c_l;&c_ja;&c_r;</trd>,
    <trd>&c_b;&c_ib;&c_z;&c_n;&c_e;&c_s;&c_m;&c_e;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>H&auml;ndler</trd>,
    <trd>Gesch&auml;ftsmann</trd>
  </trdgrp>
  <trd lng="es">comerciante</trd>
  <trdgrp lng="fr">
    <trd>commer&ccedil;ant<klr tip="ind"> (toute activit&eacute; commerciale)</klr></trd>,
    <trd>n&eacute;gociant</trd>
  </trdgrp>
</drv>

<drv mrk="komerc.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Forvendi:
      <ekz>
        eble mi devos el la vestoj ion for<tld/>i<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <adm>
    La PIVa difino aspektas preferinda.
    <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_r;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verkaufen</trd>,
    <trd>losschlagen</trd>
  </trdgrp>
  <trd lng="es">agotar<klr> (una mercanc&iacute;a)</klr></trd>
  <trdgrp lng="fr">
    <trd>&eacute;couler<klr tip="ind"> (une marchandise)</klr></trd>
  </trdgrp>
  <trd lng="nl">verkopen</trd>
</drv>
</art>
