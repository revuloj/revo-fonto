<drv mrk="komerc.0a">
  <kap><tld/>a</kap>
  <snc mrk="komerc.0a.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>aj korporacioj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>a &ccirc;ambro</ind>
        <klr>(komitato da <tld/>istoj elektitaj por studi la
        <tld/>ajn interesojn)</klr>;
        <trd lng="fr">chambre de <ind>commerce</ind></trd>
        <trd lng="hu">kereskedelmi kamara</trd>
        <trd lng="id"><ind>kamar</ind> dagang</trd>
        <trd lng="pl">izba handlowa</trd>
      </ekz>
      <ekz>
        la vagvendisto fari&gcirc;is <tld/>a konsilano
        <fnt>
          <bib>Fab3</bib>
        </fnt>;
      </ekz>
      <ekz>
        la amiko de mia patro estis ruinigita de malsukcesintaj <tld/>aj entreprenoj
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne ta&ubreve;gas la vero por <tld/>a afero
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_m;&c_e;&c_r;&c_c;&c_y;&c_j;&c_n;&c_y;</trd>,
    <trd>&c_g;&c_a;&c_n;&c_d;&c_l;&c_jo;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">comercial</trd>
