<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bure.xml,v 1.13 2023/10/29 19:11:04 revo Exp $">
<kap>
  <rad>bure</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="bure.0o">
  <kap><tld/>o</kap>
  <snc mrk="bure.0o.danco">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Francdevena popola <ref tip="lst" cel="danc.0o" lst="voko:dancoj">danco</ref>,
      du- a&ubreve; tri-takta, anka&ubreve; e&ubreve;ropa kortega danco en la 18a
      jarcento:
      <ekz>
        kursojn pri <klr>[&#x2026;]</klr> tradiciaj dancoj <klr>[&#x2026;]</klr>
        bureo, skoti&scirc;o, ktp organizas <nom>&Jcirc;arar</nom> kaj <nom>Monik Garig</nom>
        <fnt>
        <vrk>anoncetoj</vrk>,
        <lok><url ref="http://www.esperanto.hu/eventoj/arkivo/eve-004.htm">Eventoj
        1992-04, numero 4a</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x5E03;&#x5217; <pr>b&#xF9;li&#xE8;</pr></trd>
</snc>
  <snc mrk="bure.0o.muzikajxo">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzika&jcirc;o ta&ubreve;ga por <tld/>-danco, kun tipa ritma
      &scirc;vebo meze de la takto, foje uzata kiel parto
      de <ref tip="malprt" cel="suit.0o">suito</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_r;&c_e;</trd>
  <trd lng="de">Bourr&eacute;e</trd>
  <trd lng="en">bourr&eacute;e</trd>
  <trd lng="fr">bourr&eacute;e<klr> (danse)</klr></trd>
  <trd lng="he">&mem;&#x5B4;&shin;&#x5C2;&resh;&#x5B8;&dalet;</trd>
  <trd lng="hu">bourr&eacute;e</trd>
  <trd lng="pl">bourr&eacute;e</trd>
  <trd lng="ru">&c_b;&c_u;&c_r;&c_r;&c_je; <klr tip="amb">(&c_t;&c_a;&c_n;&c_je;&c_c;)</klr></trd>
  <trd lng="uk">&c_b;&c_u;&c_r;&c_je;</trd>
</drv>
</art>
