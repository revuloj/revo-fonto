<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>bure</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="bure.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="bure.0o.danco">
    <dif>
      Franc-devena popola <ref tip="super" cel="danc.0o">danco</ref>,
      du- a&ubreve; tri-takta, i&gcirc;inta e&ubreve;ropa kortega danco en
la 18a jarcento.
    </dif>
  </snc>
  <snc mrk="bure.0o.muzikajxo">
    <dif>
      Muzika&jcirc;o ta&ubreve;ga por <tld/>-danco, kun tipa ritma
&scirc;vebo meze de la
      takto, foje uzata kiel parto de <ref tip="malprt"
cel="suit.0o">suito</ref>.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>bourr&eacute;e<klr> (danse)</klr></trd>
  </trdgrp>
</drv>
</art>

