<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ablativ</rad>/o
</kap>
<drv mrk="ablati.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">GRA</uzo>
  <snc>
    <dif>
      Kazo de deklinacio en hinde&ubreve;ropaj lingvoj, esprimanta propre la
      deirpunkton.
    </dif>
  </snc>
  <trdgrp lng="br">
    <trd>tro-lemel</trd>,
    <trd>ablativ</trd>,
  </trdgrp>
  <trd lng="de">Ablativ</trd>
  <trd lng="en">ablative</trd>
  <trd lng="es">ablativo</trd>
  <trd lng="fr">ablatif</trd>
  <trdgrp lng="hu">
    <trd>ablativus</trd>,
    <trd>hat&aacute;roz&oacute;eset</trd>
  </trdgrp>
  <trd lng="nl">ablatief</trd>
  <trd lng="pt">ablativo</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_b;&c_l;&c_a;&c_t;&c_i;&c_v;</trd>,
    <trd>&c_a;&c_b;&c_l;&c_ja;&c_t;&c_i;&c_v;</trd>,
    <trd>&c_o;&c_t;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_p;&c_a;&c_d;&c_je;&c_zh;</trd>
  </trdgrp>
  <trd lng="sv">ablativ</trd>
</drv>
</art>
