<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tet.xml,v 1.7 2024/04/01 19:20:04 revo Exp $">
<kap>
  <rad>Tet</rad>/o
</kap>
<drv mrk="tet.0o">
  <kap><tld/>o,
  <var><kap><tld/>festo</kap></var></kap>
  <snc mrk="tet.0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      <ref tip="super" cel="jar.nov0o.KAL">Novjaro</ref>,
      <ref tip="lst" cel="fest.0o.KAL" lst="voko:festoj">festo</ref>
      de la unua tago de la jaro la&ubreve; la vjetnama kalendaro:
      <ekz>
        mi redonos la veturilon kaj hejmen iros por <tld/>festumi
        <fnt>
         <aut>Nguyen Cong Hoan, trad. Nguyen Xuan Thu</aut>,
         <vrk><url ref="http://www.ipernity.com/blog/xuanautuno32/447938">
         Ge&ccirc;evaloj &ndash; homaj a&ubreve; Du rajdbestoj</url></vrk>,
         <lok>2013-02-26</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>T&#7871;t Nguy&ecirc;n &#272;&aacute;n</trd>,
      <trd>T&#7871;t</trd>
    </trdgrp>
  </snc>
    <rim>
      <ref tip="hom" cel="teto.0o">Teto</ref>
      estas anka&ubreve; litero de la greka, hebrea, fenica alfabetoj.
    </rim>
    <trd lng="be">&c_T;&c_e;&c_t; <klr>(&c_v;&#8217;&c_je;&c_t;&c_n;&c_a;&c_m;&c_s;&c_k;&c_ib; &c_N;&c_o;&c_v;&c_y; &c_g;&c_o;&c_d;)</klr></trd>
  <trd lng="en">T&#7871;t</trd>
  <trd lng="fr">T&ecirc;t</trd>
  <trd lng="it">T&#7871;t</trd>
  <trd lng="nl">Tet</trd>
  <trd lng="no">tet</trd>
  <trd lng="pl">&Sacute;wi&eogonek;to <ind>Tet</ind> <klr>(wietnamski nowy rok)</klr></trd>
  <trd lng="vi">T&#7871;t</trd>
</drv>
</art>

