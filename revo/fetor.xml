  <rad>fetor</rad>/o
  <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="fetor.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Malagrablega, na&ubreve;za <ref cel="odor.0o">odoro</ref>:
      <ekz>
        en nia &scirc;tuparejo &scirc;vebas neeltenebla fetoro&#8230; &scirc;ajne mortinta kato a&ubreve; rato putras ie
        <fnt>
          <bib>Monato</bib>
          <aut>Lena Karpunina</aut>
          <vrk><url ref="&Monato;/2008/009339.php">En granda urbo</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="odor.malbon0o">malbonodoro</ref>,
      <ref cel="haladz.0o">haladzo</ref>,
      <ref cel="miasm.0o">miasmo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_m;&c_u;&c_r;&c_o;&c_d;</trd>
  <trdgrp lng="bg">
    <trd>&c_v;&c_o;&c_n;&c_ja;</trd>,
    <trd>&c_s;&c_m;&c_r;&c_a;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>fortor</trd>,
    <trd>fetor</trd>,
    <trd>ferum</trd>,
    <trd>pesta</trd>,
    <trd>pestil&egrave;ncia</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>smrad</trd>,
    <trd>z&aacute;pach</trd>
  </trdgrp>
  <trd lng="de">Gestank</trd>
  <trdgrp lng="en">
    <trd>fetor</trd>,
    <trd>stench</trd>,
    <trd>stink</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>pestilencia</trd>,
    <trd>fetidez</trd>,
    <trd>hedor</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_b;&a_w;&f_g;&a_n;&a_d;</trd>,
    <trd>&a_b;&a_w;&f_y; &a_t;&a_ayn;&a_f;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>puanteur</trd>,
    <trd>infection<klr tip="ind"> (puanteur)</klr></trd>,
    <trd>pestilence</trd>
  </trdgrp>
  <trd lng="hu">b&udblac;z</trd>
  <trdgrp lng="ja">
    <trd>&#24746;&#33261;  <pr>&#12354;&#12367;&#12375;&#12421;&#12358;</pr></trd>,
    <trd>&#33104;&#33261;  <pr>&#12405;&#12375;&#12421;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">stank</trd>
  <trdgrp lng="pl">
    <trd>fetor</trd>,
    <trd>smr&oacute;d</trd>,
    <trd>od&oacute;r</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fedor</trd>,
    <trd>cheiro nauseabundo</trd>,
    <trd>bodum</trd>,
    <trd>catinga</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_n;&c_mol;</trd>,
    <trd>&c_s;&c_m;&c_r;&c_a;&c_d;</trd>,
    <trd>&c_z;&c_l;&c_o;&c_v;&c_o;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>smrad</trd>,
    <trd>zl&yacute; pach</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_m;&c_o;&c_r;&c_ib;&c_d;</trd>,
    <trd>&c_s;&c_o;&c_p;&c_u;&c_h;</trd>
  </trdgrp>
  <trd lng="zh">&#38590;&#38395;&#30340;&#27668;&#21619; <pr>n&aacute;nw&eacute;ndeq&igrave;w&egrave;i</pr></trd>
</drv>

<drv mrk="fetor.0a">
  <kap><tld/>a</kap>
  <snc mrk="fetor.0a.fet">
    <dif>
      Karakterizata de fetoro:
      <ekz>
        oni trenis ilin tra la fetora flua&jcirc;o de la stratkanaletoj
        <fnt>
          <aut>B. Traven, trad. Hans Georg Kaiser</aut>,
          <vrk><url ref="http://www.karapaco.de/u/B.Traven/btraven_mortulasxipo.pdf">Mortula
          &scirc;ipo</url></vrk>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_m;&c_ja;&c_r;&c_d;&c_z;&c_ju;&c_ch;&c_y;</trd>,
    <trd>&c_s;&c_m;&c_u;&c_r;&c_o;&c_d;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_v;&c_o;&c_n;&c_ja;&c_shch;</trd>,
    <trd>&c_s;&c_m;&c_malmol;&c_r;&c_d;&c_ja;&c_shch;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>pudent</trd>,
    <trd>pestilent</trd>,
    <trd>f&egrave;tid</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>p&aacute;chnouc&iacute;</trd>,
    <trd>smrdut&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>stinkend</trd>,
    <trd>stinkig</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>fetid</trd>,
    <trd>malodorous</trd>,  
    <trd>stinky</trd>,     
    <trd>bad-smelling</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>f&eacute;tido</trd>,
    <trd>f&eacute;tida</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_m;&a_t;&a_ayn;&a_f;&a_n;</trd>,
    <trd>&a_b;&a_d;&a_b;&a_w;</trd>
  </trdgrp>
  <trd lng="fr">f&eacute;tide</trd>
  <trdgrp lng="ja">
    <trd>&#24746;&#33261;&#12398;&#12402;&#12393;&#12356;  <pr>&#12354;&#12367;&#12375;&#12421;&#12358;&#12398;&#12402;&#12393;&#12356;</pr></trd>,
    <trd>&#12416;&#12363;&#12388;&#12367;&#12424;&#12358;&#12394;&#12395;&#12362;&#12356;&#12398;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&sacute;mierdz&aogonek;cy</trd>,
    <trd>cuchn&aogonek;cy</trd>
  </trdgrp>
  <trd lng="sk">p&aacute;chnuci</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_m;&c_je;&c_r;&c_d;&c_ju;&c_ch;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_m;&c_je;&c_r;&c_d;&c_ja;&c_ch;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#33261;&#30340; <pr>ch&ograve;ude</pr></trd>,
    <trd>&#24758; <pr>m&#275;n</pr></trd>,
    <trd>&#33261;&#28888;&#28888; <pr>ch&ograve;uh&#333;ngh&#333;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fetor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Eligi a&ccirc;an odoron:
      <ekz>
        la koridoro <tld/>is pro boligitaj brasikoj
        <fnt>
          <aut>George Orwell, trad. Donald Broadribb</aut>,
          <vrk><url
          ref="http://i-espero.info/files/elibroj/eo%20-%20orwell,%20george%20-%201984.pdf">Mil
          Na&ubreve;cent Okdek Kvar</url></vrk>,
          <lok>p. 1</lok>
        </fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="stink.0i">stinki</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_m;&c_ja;&c_r;&c_d;&c_z;&c_je;&c_c;&c_mol;</trd>
  <trdgrp lng="bg">
    <trd>&c_v;&c_o;&c_n;&c_ja;</trd>,
    <trd>&c_s;&c_m;&c_malmol;&c_r;&c_d;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>fer pudor</trd>,
    <trd>fer ferum</trd>,
    <trd>pudir</trd>,
    <trd>cantar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>p&aacute;chnout</trd>,
    <trd>smrd&ecaron;t</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>stinken</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>reek</trd>,
    <trd>stink</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>apestar</trd>,
    <trd>heder</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_b;&a_w;&f_g;&a_n;&a_d; &a_d;&a_A;&a_d;&a_n;</trd>,
    <trd>&a_b;&a_w;&f_y; &a_b;&a_d; &a_d;&a_A;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>puer</trd>,
    <trd>empester</trd>,
    <trd>sentir mauvais</trd>
  </trdgrp>
  <trd lng="hu">b&udblac;zlik</trd>
  <trd lng="ja">&#24746;&#33261;&#12434;&#25918;&#12388;  <pr>&#12354;&#12367;&#12375;&#12421;&#12358;&#12434;&#12399;&#12394;&#12388;</pr></trd>
  <trd lng="nl">stinken</trd>
  <trdgrp lng="pl">
    <trd>&sacute;mierdzie&cacute;</trd>,
    <trd>cuchn&aogonek;&cacute;</trd>,
    <trd>wonie&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_n;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_m;&c_je;&c_r;&c_d;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>p&aacute;chnu&tcaron;</trd>,
    <trd>smrdie&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_m;&c_je;&c_r;&c_d;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_r;&c_i;&c_jeu;&c_m;&c_n;&c_o; &c_p;&c_a;&c_h;&c_n;&c_u;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#33261;&#20046;&#20046; <pr>ch&ograve;uh&#363;h&#363;</pr></trd>
</drv>
