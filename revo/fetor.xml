<vortaro>
<art mrk="$Id: fetor.xml,v 1.35 2023/11/09 19:13:17 revo Exp $">
<kap>
  <rad>fetor</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="fetor.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Malagrablega, na&ubreve;za <ref cel="odor.0o">odoro</ref>:
      <ekz>
        en nia &scirc;tuparejo &scirc;vebas neeltenebla fetoro&#x2026; &scirc;ajne mortinta kato a&ubreve; rato putras ie
        <fnt>
          <bib>Monato</bib>
          <aut>Lena Karpunina</aut>
          <vrk><url ref="&Monato;/2008/009339.php">En granda urbo</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="odor.malbon0o">malbonodoro</ref>,
      <ref cel="haladz.0o">haladzo</ref>,
      <ref cel="miasm.0o">miasmo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_m;&c_u;&c_r;&c_o;&c_d;</trd>
  <trdgrp lng="bg">
    <trd>&c_v;&c_o;&c_n;&c_ja;</trd>,
    <trd>&c_s;&c_m;&c_r;&c_a;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>smrad</trd>,
    <trd>z&aacute;pach</trd>
  </trdgrp>
  <trd lng="de">Gestank</trd>
  <trdgrp lng="en">
    <trd>fetor</trd>,
    <trd>stench</trd>,
    <trd>stink</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>pestilencia</trd>,
    <trd>fetidez</trd>,
    <trd>hedor</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_b;&a_w;&f_g;&a_n;&a_d;</trd>,
    <trd>&a_b;&a_w;&f_y; &a_t;&a_ayn;&a_f;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>puanteur</trd>,
    <trd>infection<klr tip="ind"> (puanteur)</klr></trd>,
    <trd>pestilence</trd>
  </trdgrp>
  <trd lng="hu">b&udblac;z</trd>
  <trdgrp lng="ja">
    <trd>&#x60AA;&#x81ED;  <pr>&#x3042;&#x304F;&#x3057;&#x3085;&#x3046;</pr></trd>,
    <trd>&#x8150;&#x81ED;  <pr>&#x3075;&#x3057;&#x3085;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">stank</trd>
  <trdgrp lng="pl">
    <trd>fetor</trd>,
    <trd>smr&oacute;d</trd>,
    <trd>od&oacute;r</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fedor</trd>,
    <trd>cheiro nauseabundo</trd>,
    <trd>bodum</trd>,
    <trd>catinga</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_n;&c_mol;</trd>,
    <trd>&c_s;&c_m;&c_r;&c_a;&c_d;</trd>,
    <trd>&c_z;&c_l;&c_o;&c_v;&c_o;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>smrad</trd>,
    <trd>zl&yacute; pach</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43C;&#x43E;&c_r;&c_ib;&c_d;</trd>,
    <trd>&c_s;&#x43E;&#x43F;&c_u;&c_h;</trd>
  </trdgrp>
  <trd lng="zh">&#x96BE;&#x95FB;&#x7684;&#x6C14;&#x5473; <pr>n&#xE1;nw&#xE9;ndeq&#xEC;w&#xE8;i</pr></trd>
</drv>

<drv mrk="fetor.0a">
  <kap><tld/>a</kap>
  <snc mrk="fetor.0a.fet">
    <dif>
      Karakterizata de fetoro:
      <ekz>
        oni trenis ilin tra la fetora flua&jcirc;o de la stratkanaletoj
        <fnt>
          <aut>B. Traven, trad. Hans Georg Kaiser</aut>,
          <vrk><url ref="http://www.karapaco.de/u/B.Traven/btraven_mortulasxipo.pdf">Mortula
          &scirc;ipo</url></vrk>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_m;&c_ja;&c_r;&c_d;&c_z;&c_ju;&c_ch;&c_y;</trd>,
    <trd>&c_s;&c_m;&c_u;&c_r;&c_o;&c_d;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_v;&c_o;&c_n;&c_ja;&c_shch;</trd>,
    <trd>&c_s;&c_m;&c_malmol;&c_r;&c_d;&c_ja;&c_shch;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>p&aacute;chnouc&iacute;</trd>,
    <trd>smrdut&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>stinkend</trd>,
    <trd>stinkig</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>fetid</trd>,
    <trd>malodorous</trd>,  
    <trd>stinky</trd>,     
    <trd>bad-smelling</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>f&eacute;tido</trd>,
    <trd>f&eacute;tida</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_m;&a_t;&a_ayn;&a_f;&a_n;</trd>,
    <trd>&a_b;&a_d;&a_b;&a_w;</trd>
  </trdgrp>
  <trd lng="fr">f&eacute;tide</trd>
  <trdgrp lng="ja">
    <trd>&#x60AA;&#x81ED;&#x306E;&#x3072;&#x3069;&#x3044;  <pr>&#x3042;&#x304F;&#x3057;&#x3085;&#x3046;&#x306E;&#x3072;&#x3069;&#x3044;</pr></trd>,
    <trd>&#x3080;&#x304B;&#x3064;&#x304F;&#x3088;&#x3046;&#x306A;&#x306B;&#x304A;&#x3044;&#x306E;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&sacute;mierdz&aogonek;cy</trd>,
    <trd>cuchn&aogonek;cy</trd>
  </trdgrp>
  <trd lng="sk">p&aacute;chnuci</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43C;&c_je;&c_r;&c_d;&#x44E;&c_ch;&c_i;&c_j;</trd>,
    <trd>&c_s;&#x43C;&c_je;&c_r;&c_d;&#x44F;&c_ch;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x81ED;&#x7684; <pr>ch&#xF2;ude</pr></trd>,
    <trd>&#x60B6; <pr>m&#x113;n</pr></trd>,
    <trd>&#x81ED;&#x70D8;&#x70D8; <pr>ch&#xF2;uh&#x14D;ngh&#x14D;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fetor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Eligi a&ccirc;an odoron:
      <ekz>
        la koridoro <tld/>is pro boligitaj brasikoj
        <fnt>
          <aut>George Orwell, trad. Donald Broadribb</aut>,
          <vrk><url ref="http://i-espero.info/files/elibroj/eo%20-%20orwell,%20george%20-%201984.pdf">Mil
          Na&ubreve;cent Okdek Kvar</url></vrk>,
          <lok>p. 1</lok>
        </fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="stink.0i">stinki</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_m;&c_ja;&c_r;&c_d;&c_z;&c_je;&c_c;&c_mol;</trd>
  <trdgrp lng="bg">
    <trd>&c_v;&c_o;&c_n;&c_ja;</trd>,
    <trd>&c_s;&c_m;&c_malmol;&c_r;&c_d;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>p&aacute;chnout</trd>,
    <trd>smrd&ecaron;t</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>stinken</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>reek</trd>,
    <trd>stink</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>apestar</trd>,
    <trd>heder</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_b;&a_w;&f_g;&a_n;&a_d; &a_d;&a_A;&a_d;&a_n;</trd>,
    <trd>&a_b;&a_w;&f_y; &a_b;&a_d; &a_d;&a_A;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>puer</trd>,
    <trd>empester</trd>,
    <trd>sentir mauvais</trd>
  </trdgrp>
  <trd lng="hu">b&udblac;zlik</trd>
  <trd lng="ja">&#x60AA;&#x81ED;&#x3092;&#x653E;&#x3064;  <pr>&#x3042;&#x304F;&#x3057;&#x3085;&#x3046;&#x3092;&#x306F;&#x306A;&#x3064;</pr></trd>
  <trd lng="nl">stinken</trd>
  <trdgrp lng="pl">
    <trd>&sacute;mierdzie&cacute;</trd>,
    <trd>cuchn&aogonek;&cacute;</trd>,
    <trd>wonie&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_n;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_m;&c_je;&c_r;&c_d;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>p&aacute;chnu&tcaron;</trd>,
    <trd>smrdie&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43C;&c_je;&c_r;&c_d;&c_ib;&c_t;&c_i;</trd>,
    <trd>&#x43D;&c_je;&#x43F;&c_r;&c_i;&c_jeu;&#x43C;&#x43D;&#x43E; &#x43F;&c_a;&c_h;&#x43D;&c_u;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x81ED;&#x4E4E;&#x4E4E; <pr>ch&#xF2;uh&#x16B;h&#x16B;</pr></trd>
</drv>
</art>
