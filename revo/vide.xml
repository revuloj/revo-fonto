<art mrk="$Id: vide.xml,v 1.16 2025/08/10 20:33:39 revo Exp $">
<kap>
  <rad>vide</rad>/o <fnt><bib>RokPop</bib></fnt>
</kap>
<drv mrk="vide.0o">
  <kap><tld/>o</kap>
  <snc mrk="vide.0o.KIN">
    <uzo tip="fak">KIN</uzo>
    <dif>
      <ref tip="lst" cel="art.0o.tehxniko" lst="voko:artoj">Arto</ref>
      pri elektronika registro, prilaboro a&ubreve; telekomuniko
      de movi&gcirc;aj bildoj; <ref tip="sin" cel="art.vide0o.KIN">videarto</ref>:
      <ekz>
        ni havis okazon vidi kaj a&ubreve;skulti per <tld/>o la faman operon
        &leftquot;<nom>La Boh&egrave;me</nom>&rightquot;
        <fnt>
          <aut>M. R. Urue&ntilde;a</aut>,
          <vrk>Internacia E-Semajno de la Kulturo kaj Turismo</vrk>,
          <lok><url ref="http://www.esperanto.es">esperanto.es</url>,
          [vidita en 2010-11]</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni spektas vidbendon, a&ubreve; <tld/>oludas, a&ubreve; konsultas teletekston
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2003/008062.html">Spektantoj spektataj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Liza</nom> montris al mi novan <ref cel="film.vide0o">videofilmon</ref>
        pri la fama &ccirc;ina cirko
        <fnt>
          <bib>Monato</bib>
          <aut>Lena Karpunina</aut>
          <vrk><url ref="&Monato;/2006/008836.php">La revo pri cirko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ne havas <tld/>oaparaton por projekcii al li ian infanan bildfilmon
        <fnt>
          <bib>Monato</bib>
          <aut>Julian Modest</aut>
          <vrk><url ref="&Monato;/2007/009024.php">Bela son&gcirc;o</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&ccirc;ar]</klr> preska&ubreve; &ccirc;iu jam havas hejme <tld/>omagnetofonon, ne necesas
        viziti kinejojn: &ccirc;ion eblas rigardi hejme
        <fnt>
          <bib>Monato</bib>
          <aut>Dmitrij Cibulevskij</aut>
          <vrk><url ref="&Monato;/2007/009112.php">Busvoja&gcirc;o</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi lastatempe ricevis <nom>Macbook</nom>-on de mia laborejo, kaj &gcirc;i
        inkluzivas kameraon por <tld/>obabili
        <klr>(vd <ref cel="kamera.vide0o">videkamerao</ref>)</klr>
        <fnt>
          <aut>&Gcirc;eremio</aut>,
          <vrk><url ref="http://esperanto-usa.org/eo/node/478">
          Cxu iu volas video-babili?</url></vrk>,
          <lok>[2006]</lok>
        </fnt>;
      </ekz>
      <ekz>
        per <ref cel="konfer.video0o">videokonferenco</ref>
        parolis el Usono profesoro <nom>Sami Repishti</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2011/010506.php">Murdoj memorigitaj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Videotechnik</trd>
    <trd lng="ja">&#x30D3;&#x30C7;&#x30AA;&#26144;&#x50CF;&#25216;&#34899; 
    <pr>&#12403;&#12391;&#x304A;&#12360;&#12356;&#x305E;&#12358;&#x304E;&#12376;&#12421;&#12388;</pr></trd>
    <trdgrp lng="nl">
      <trd>video</trd>,
      <trd>videotechniek</trd>
    </trdgrp>
  </snc>
  <snc mrk="vide.0o.KIN_videajxo">
    <uzo tip="fak">KIN</uzo>
    <dif>
      <ref tip="dif" cel="vide.0ajxo.KIN">Videa&jcirc;o</ref>:
      <ekz>
        kiel rapide &ccirc;e vi rul<klr>[i&gcirc;]</klr>as <tld/>oj en <nom>YouTube</nom>
        <fnt>
          <aut>Katjo</aut>,
          <vrk><url
          ref="http://blogo.ikso.net/2010/02/kiel-rapida-estas-via-konektigxo-al-youtube-testu-gxin/">
          Kiel rapida estas via konekti&gcirc;o al YouTube?</url></vrk>,
          <lok>E@i, 2010-02-15</lok>
        </fnt>?
      </ekz>
      <ekz>
        la pa&gcirc;aro proponas sian spacon por jenaj tipoj de materialoj: informiloj, fotoj, bildoj,
        <tld/>oj kaj retligiloj
        <fnt>
          <aut>Peter Bal&aacute;&zcaron;</aut>
          <vrk>Reta Informado-Centro reaperas</vrk>
          <bib>Esperanto</bib>
          <lok>2006 7+8</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekzistas nuntempe pli kaj pli multnombraj esperantistaj jutubistoj, kiuj faras <tld/>ojn pri
        kaj en Esperanto
        <fnt>
          <aut>K&eacute;vin Morel-Fourrier</aut>
          <vrk>Linguisticae, la lingva jutubisto kiu apogas Esperanton</vrk>
          <bib>Kon11</bib>
        </fnt>
      </ekz>
      <ekz>
        a&ubreve;dregistra&jcirc;oj, <tld/>ofragmentoj kaj cita&jcirc;oj de samtempuloj
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2011/010480.php">Verda vojo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Video</trd>
    <trdgrp lng="ja">
      <trd>&#x30D3;&#x30C7;&#x30AA;&#26144;&#x50CF;  <pr>&#12403;&#12391;&#x304A;&#12360;&#12356;&#x305E;&#12358;</pr></trd>,
      <trd>&#12499;&#12487;&#12458;</trd>
      <trd>&#21205;&#30011; <pr>&#12393;&#12358;&#12364;</pr></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>video</trd>,
      <trd>filmpje</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_d;&c_e;&c_a;</trd>
  <trd lng="cs">video</trd>
  <trd lng="en">video<klr> (n)</klr></trd>
  <trd lng="es">video</trd>
  <trd lng="fr">vid&eacute;o</trd>
  <trd lng="he">&vav;&yod;&dalet;&alef;&vav;</trd>
  <trd lng="id">video</trd>
  <trd lng="sk">video</trd>
  <trdgrp lng="zh">
    <trd>&#35222;&#x983B; <pr>sh&#xEC;p&#xED;n</pr></trd>,
    <trd>&#x89C6;&#39057; <pr>sh&#xEC;p&#xED;n</pr></trd>,
    <trd>&#x52A8;&#24577;&#x5F71;&#x50CF; <pr>d&#xF2;ngt&#xE0;iy&#x1D0;ngxi&#xE0;ng</pr></trd>,
    <trd>&#x52D5;&#x614B;&#x5F71;&#x50CF; <pr>d&#xF2;ngt&#xE0;iy&#x1D0;ngxi&#xE0;ng</pr></trd>,
    <trd>&#35222;&#x50CF; <pr>sh&#xEC;xi&#xE0;ng</pr></trd>,
    <trd>&#x89C6;&#x50CF; <pr>sh&#xEC;xi&#xE0;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="vide.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="vide.0ajxo.KIN">
    <uzo tip="fak">KIN</uzo>
    <dif>
      <ref tip="super" cel="film.0o">Filmo</ref> a&ubreve; alia produkto de <tld/>arto:
      <ekz>
        multaj homoj, precipe la gejunuloj, emas uzi fotilojn a&ubreve; po&scirc;telefonojn por fari <tld/>a&jcirc;ojn
        por sin amuzi
        <fnt>
          <aut>Xiao Huoli</aut>
          <vrk>E-filmfestivalo kaptis la atenton de neesperantistoj</vrk>
          <bib>Esperanto</bib>
          <lok>2014 5</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a&jcirc;o pri la Zamenhofa tago 2016 en Ljubljano
        <fnt>
          <vrk><url
          ref="https://esperanto.si/eo/videajxo-pri-la-zamenhofa-tago-2016-en-ljubljano">Videa&jcirc;o pri
          la Zamenhofa tago 2016 en Ljubljano</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a&jcirc;o de tiu ago estas spektebla e&ccirc; kun anglalingvaj
        subtekstoj
        <fnt>
          <aut>blahma</aut>,
          <vrk><url ref="http://blahma.livejournal.com/13202.html?thread=35730">
          Parlamenta baloto en &Ccirc;e&hcirc;io</url></vrk>,
          <lok>2006-06-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ib;&c_d;&c_e;&c_a;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_e;&c_a;&c_s;&c_t;&c_u;&c_zh;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_e;&c_a;&c_f;&c_ib;&c_l;&c_mol;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Video</trd>,
    <trd>Filmclip</trd>
  </trdgrp>
  <trd lng="en">video<klr> (n)</klr></trd>
  <trdgrp lng="es">
    <trd>v&iacute;deo</trd>,
    <trd>video</trd>
  </trdgrp>
  <trd lng="fr">vid&eacute;o<klr> (film, produit vid&eacute;o)</klr></trd>
  <trd lng="id">video</trd>
  <trd lng="ja">&#x30D3;&#x30C7;&#x30AA;&#26144;&#x50CF;  <pr>&#12403;&#12391;&#x304A;&#12360;&#12356;&#x305E;&#12358;</pr></trd>
  <trdgrp lng="nl">
    <trd>video</trd>,
    <trd>filmpje</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5F55;&#21046;&#x89C6;&#39057;&#29255;&#x65AD;
    <pr>l&#xF9;zh&#xEC;sh&#xEC;p&#xED;npi&#xE0;ndu&#xE0;n</pr></trd>,
    <trd>&#37636;&#x88FD;&#35222;&#x983B;&#29255;&#x65B7;
    <pr>l&#xF9;zh&#xEC;sh&#xEC;p&#xED;npi&#xE0;ndu&#xE0;n</pr></trd>,
    <trd>&#x5F55;&#x50CF; <pr>l&#xF9;xi&#xE0;ng</pr></trd>,
    <trd>&#37636;&#x50CF; <pr>l&#xF9;xi&#xE0;ng</pr></trd>,
    <trd>&#x52A8;&#24577;&#x5F71;&#x50CF; <pr>d&#xF2;ngt&#xE0;iy&#x1D0;ngxi&#xE0;ng</pr></trd>,
    <trd>&#x52D5;&#x614B;&#x5F71;&#x50CF; <pr>d&#xF2;ngt&#xE0;iy&#x1D0;ngxi&#xE0;ng</pr></trd>,
    <trd>&#35222;&#x983B;&#x6D88;&#x606F; <pr>sh&#xEC;p&#xED;nxi&amacron;ox&#xED;</pr></trd>,
    <trd>&#x89C6;&#39057;&#x6D88;&#x606F; <pr>sh&#xEC;p&#xED;nxi&amacron;ox&#xED;</pr></trd>,
    <trd>&#35222;&#x983B;&#37636;&#x50CF; <pr>sh&#xEC;p&#xED;nl&#xF9;xi&#xE0;ng</pr></trd>,
    <trd>&#x89C6;&#39057;&#x5F55;&#x50CF; <pr>sh&#xEC;p&#xED;nl&#xF9;xi&#xE0;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="vide.muzik0o">
  <kap>
    muzik<tld/>o,
    <var><kap>muzik<tld/>a&jcirc;o</kap></var>
  </kap>
  <snc mrk="vide.muzik0o.MUZ">
    <uzo tip="fak">KIN</uzo>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <tld lit="V"/>a&jcirc;o prezentanta muzika&jcirc;on kun akompana filmo:
      <ekz>
        &ccirc;u vi povas rakonti pri tiu muzik<tld/>o, kiel &gcirc;i esti&gcirc;is?
        <fnt>
          <aut>Rogener Pavinski</aut>
          <vrk>Ale Kossabela &mdash; Kameleono de la E-muziko</vrk>
          <bib>Kon11</bib>
        </fnt>;
      </ekz>
      <ekz>
        la oficiala muzik<tld/>a&jcirc;o estas bele farita, konforme al la titolo en formo de mozaiko
        <fnt>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="https://www.monato.be/2021/012883.php">Promesplena da&ubreve;rigo</url></vrk>
          <bib>Monato</bib>
          <lok>2021/12, p. 26</lok>
        </fnt>;
      </ekz>
      <ekz>
        estis aldonita muzik<tld/>o de la kanto
        <ctl>Berlino sen vi</ctl>
        <fnt>
          <aut>Vinilkosmo</aut>,
          <vrk><url ref="http://www.vinilkosmo.com/?disc=vkkd103">
          Urbano</url></vrk>,
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bela surprizo estas, ke la muzik<tld/>a&jcirc;o kantita en Esperanto estas subtekstigita jam
        per 10 lingvoj, inkluzive de la pola
        <fnt>
          <aut>Pola Retradio</aut>
          <vrk><url
          ref="https://pola-retradio.org/2020/02/la-pordo-de-ale-cosabela-kaj-vinilkosmo/">La Pordo
          de Ale Cosabela kaj Vinilkosmo</url></vrk>
          <lok>2020-02-28</lok>
        </fnt>;
      </ekz>
      <ekz>
        la unua profesia muzik<tld/>o de la
        nova Esperanto-muzikgrupo Supernova
        <fnt>
          <vrk><url ref="https://vimeo.com/1234545">Supernova &dash; Pasio en Katen' on Vimeo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_z;&c_y;&c_ch;&c_n;&c_y; <ind>&c_k;&c_l;&c_ib;&c_p;</ind></trd>
  <trd lng="de">Musikvideo</trd>
  <trd lng="en">music video</trd>
  <trdgrp lng="es">
    <trd>videoclip</trd>,
    <trd>v&iacute;deo musical</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>clip</trd>,
    <trd>vid&eacute;o musicale</trd>
  </trdgrp>
  <trd lng="id"><ind>video</ind> klip</trd>
  <trdgrp lng="ja">
    <trd>&#x30DF;&#x30E5;&#x30FC;&#x30B8;&#x30C3;&#x30AF;&#x30D3;&#x30C7;&#x30AA;</trd>,
    <trd>&#x97F3;&#x697D;&#x30D3;&#x30C7;&#x30AA; 
    <pr>&#x304A;&#12435;&#x304C;&#x304F;&#12403;&#12391;&#x304A;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>muziekvideo</trd>,
    <trd>videoclip</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x97F3;&#x4E50;&#30005;&#x89C6; <pr>y&#x12B;nyu&#xE8;di&#xE0;nsh&#xEC;</pr></trd>,
    <trd>&#x97F3;&#x6A02;&#x96FB;&#35222; <pr>y&#x12B;nyu&#xE8;di&#xE0;nsh&#xEC;</pr></trd>,
    <trd>&#x97F3;&#x4E50;&#x5F55;&#x5F71;&#x5E26;
    <pr>y&#x12B;nyu&#xE8;l&#xF9;y&#x1D0;ngd&#xE0;i</pr></trd>,
    <trd>&#x97F3;&#x6A02;&#37636;&#x5F71;&#x5E36; <pr>y&#x12B;nyu&#xE8;l&#xF9;y&#x1D0;ngd&#xE0;i</pr></trd>
  </trdgrp>
</drv>
