<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vide.xml,v 1.13 2023/10/21 16:50:36 revo Exp $">
<kap>
  <rad>vide</rad>/o <fnt><bib>RokPop</bib></fnt>
</kap>
<drv mrk="vide.0o">
  <kap><tld/>o</kap>
  <snc mrk="vide.0o.KIN">
    <uzo tip="fak">KIN</uzo>
    <dif>
      <ref tip="lst" cel="art.0o.tehxniko" lst="voko:artoj">Arto</ref>
      pri elektronika registro, prilaboro a&ubreve; telekomuniko
      de movi&gcirc;aj bildoj; <ref tip="sin" cel="art.vide0o.KIN">videarto</ref>:
      <ekz>
        ni havis okazon vidi kaj a&ubreve;skulti per <tld/>o la faman operon
        &leftquot;<nom>La Boh&egrave;me</nom>&rightquot;
        <fnt>
          <aut>M. R. Urue&ntilde;a</aut>,
          <vrk>Internacia E-Semajno de la Kulturo kaj Turismo</vrk>,
          <lok><url ref="http://www.esperanto.es">esperanto.es</url>,
          [vidita en 2010-11]</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni spektas vidbendon, a&ubreve; <tld/>oludas, a&ubreve; konsultas teletekston
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2003/008062.html">Spektantoj spektataj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Liza</nom> montris al mi novan <ref cel="film.vide0o">videofilmon</ref>
        pri la fama &ccirc;ina cirko
        <fnt>
          <bib>Monato</bib>
          <aut>Lena Karpunina</aut>
          <vrk><url ref="&Monato;/2006/008836.php">La revo pri cirko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ne havas <tld/>oaparaton por projekcii al li ian infanan bildfilmon
        <fnt>
          <bib>Monato</bib>
          <aut>Julian Modest</aut>
          <vrk><url ref="&Monato;/2007/009024.php">Bela son&gcirc;o</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&ccirc;ar]</klr> preska&ubreve; &ccirc;iu jam havas hejme <tld/>omagnetofonon, ne necesas
        viziti kinejojn: &ccirc;ion eblas rigardi hejme
        <fnt>
          <bib>Monato</bib>
          <aut>Dmitrij Cibulevskij</aut>
          <vrk><url ref="&Monato;/2007/009112.php">Busvoja&gcirc;o</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi lastatempe ricevis <nom>Macbook</nom>-on de mia laborejo, kaj &gcirc;i
        inkluzivas kameraon por <tld/>obabili
        <klr>(vd <ref cel="kamera.vide0o">videkamerao</ref>)</klr>
        <fnt>
          <aut>&Gcirc;eremio</aut>,
          <vrk><url ref="http://esperanto-usa.org/eo/node/478">
          Cxu iu volas video-babili?</url></vrk>,
          <lok>[2006]</lok>
        </fnt>;
      </ekz>
      <ekz>
        per <ref cel="konfer.video0o">videokonferenco</ref>
        parolis el Usono profesoro <nom>Sami Repishti</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2011/010506.php">Murdoj memorigitaj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Videotechnik</trd>
  </snc>
  <snc mrk="vide.0o.KIN_videajxo">
    <uzo tip="fak">KIN</uzo>
    <dif>
      <ref tip="dif" cel="vide.0ajxo.KIN">Videa&jcirc;o</ref>:
      <ekz>
        kiel rapide &ccirc;e vi rul<klr>[i&gcirc;]</klr>as <tld/>oj en <nom>YouTube</nom>
        <fnt>
          <aut>Katjo</aut>,
          <vrk><url ref="http://blogo.ikso.net/2010/02/kiel-rapida-estas-via-konektigxo-al-youtube-testu-gxin/">
          Kiel rapida estas via konekti&gcirc;o al YouTube?</url></vrk>,
          <lok>E@i, 2010-02-15</lok>
        </fnt>?
      </ekz>
      <ekz>
        a&ubreve;dregistra&jcirc;oj, <tld/>ofragmentoj kaj cita&jcirc;oj de samtempuloj
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2011/010480.php">Verda vojo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Video</trd>
  </snc>
  <trd lng="cs">video</trd>
  <trd lng="en">video<klr> (n)</klr></trd>
  <trdgrp lng="es">
    <trd>v&iacute;deo</trd>,
    <trd>video</trd>
  </trdgrp>
  <trd lng="fr">vid&eacute;o</trd>
  <trd lng="he">&vav;&yod;&dalet;&alef;&vav;</trd>
  <trd lng="id">video</trd>
  <trdgrp lng="ja">
    <trd>&#12499;&#12487;&#12458;&#26144;&#20687;&#25216;&#34899;  <pr>&#12403;&#12391;&#12362;&#12360;&#12356;&#12382;&#12358;&#12366;&#12376;&#12421;&#12388;</pr></trd>,
    <trd>&#12499;&#12487;&#12458;&#26144;&#20687;  <pr>&#12403;&#12391;&#12362;&#12360;&#12356;&#12382;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="sk">video</trd>
</drv>

<drv mrk="vide.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="vide.0ajxo.KIN">
    <uzo tip="fak">KIN</uzo>
    <dif>
      Filmo a&ubreve; alia produkto de <tld/>arto:
      <ekz>
        <tld/>a&jcirc;o pri la Zamenhofa tago 2016 en Ljubljano
        <fnt>
          <vrk><url ref="https://esperanto.si/eo/videajxo-pri-la-zamenhofa-tago-2016-en-ljubljano">Videa&jcirc;o pri la Zamenhofa tago 2016 en Ljubljano</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a&jcirc;o de tiu ago estas spektebla e&ccirc; kun anglalingvaj
        subtekstoj
        <fnt>
          <aut>blahma</aut>,
          <vrk><url ref="http://blahma.livejournal.com/13202.html?thread=35730">
          Parlamenta baloto en &Ccirc;e&hcirc;io</url></vrk>,
          <lok>2006-06-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Video</trd>,
    <trd>Filmclip</trd>
  </trdgrp>
  <trd lng="en">video<klr> (n)</klr></trd>
  <trdgrp lng="es">
    <trd>v&iacute;deo</trd>,
    <trd>video</trd>
  </trdgrp>
  <trd lng="fr">vid&eacute;o<klr> (film, produit vid&eacute;o)</klr></trd>
  <trd lng="id">video</trd>
</drv>

<drv mrk="vide.muzik0o">
  <kap>muzik<tld/>o</kap>
  <snc mrk="vide.muzik0o.MUZ">
    <uzo tip="fak">KIN</uzo>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <tld lit="V"/>o<sncref ref="vide.0o.KIN_videajxo"/>
      prezentanta muzika&jcirc;on kun akompana filmo:
      <ekz>
        estis aldonita muzik<tld/>o de la kanto
        <ctl>Berlino sen vi</ctl>
        <fnt>
          <aut>Vinilkosmo</aut>,
          <vrk><url ref="http://www.vinilkosmo.com/?disc=vkkd103">
          Urbano</url></vrk>,
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        la unua profesia muzik<tld/>o de la
        nova Esperanto-muzikgrupo Supernova
        <fnt>
          <vrk><url ref="https://vimeo.com/1234545">Supernova &dash; Pasio en Katen' on Vimeo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Musikvideo</trd>
  <trd lng="en">music video</trd>
  <trdgrp lng="es">
    <trd>videoclip</trd>,
    <trd>v&iacute;deo musical</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>clip</trd>,
    <trd>vid&eacute;o musicale</trd>
  </trdgrp>
  <trd lng="id"><ind>video</ind> klip</trd>
  <trdgrp lng="ja">
    <trd>&#12511;&#12517;&#12540;&#12472;&#12483;&#12463;&#12499;&#12487;&#12458;</trd>,
    <trd>&#38899;&#27005;&#12499;&#12487;&#12458;  <pr>&#12362;&#12435;&#12364;&#12367;&#12403;&#12391;&#12362;</pr></trd>
  </trdgrp>
</drv>
</art>

