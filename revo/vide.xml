  <trdgrp lng="ja">
    <trd>&#x30D3;&#x30C7;&#x30AA;&#x6620;&#x50CF;&#x6280;&#x8853;  <pr>&#x3073;&#x3067;&#x304A;&#x3048;&#x3044;&#x305E;&#x3046;&#x304E;&#x3058;&#x3085;&#x3064;</pr></trd>,
    <trd>&#x30D3;&#x30C7;&#x30AA;&#x6620;&#x50CF;  <pr>&#x3073;&#x3067;&#x304A;&#x3048;&#x3044;&#x305E;&#x3046;</pr></trd>
