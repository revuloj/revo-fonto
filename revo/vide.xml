<drv mrk="vide.0o">
  <kap><tld/>o</kap>
  <snc mrk="vide.0o.KIN">
    <uzo tip="fak">KIN</uzo>
    <dif>
      <ref tip="lst" cel="art.0o.tehxniko" lst="voko:artoj">Arto</ref>
      pri elektronika registro, prilaboro a&ubreve; telekomuniko
      de movi&gcirc;aj bildoj; <ref tip="sin" cel="art.vide0o.KIN">videarto</ref>:
      <ekz>
        ni havis okazon vidi kaj a&ubreve;skulti per <tld/>o la faman operon
        &leftquot;<nom>La Boh&egrave;me</nom>&rightquot;
        <fnt>
          <aut>M. R. Urue&ntilde;a</aut>,
          <vrk>Internacia E-Semajno de la Kulturo kaj Turismo</vrk>,
          <lok><url ref="http://www.esperanto.es">esperanto.es</url>,
          [vidita en 2010-11]</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni spektas vidbendon, a&ubreve; <tld/>oludas, a&ubreve; konsultas teletekston
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2003/008062.html">Spektantoj spektataj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Liza</nom> montris al mi novan <ref cel="film.vide0o">videofilmon</ref>
        pri la fama &ccirc;ina cirko
        <fnt>
          <bib>Monato</bib>
          <aut>Lena Karpunina</aut>
          <vrk><url ref="&Monato;/2006/008836.php">La revo pri cirko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ne havas <tld/>oaparaton por projekcii al li ian infanan bildfilmon
        <fnt>
          <bib>Monato</bib>
          <aut>Julian Modest</aut>
          <vrk><url ref="&Monato;/2007/009024.php">Bela son&gcirc;o</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&ccirc;ar]</klr> preska&ubreve; &ccirc;iu jam havas hejme <tld/>omagnetofonon, ne necesas
        viziti kinejojn: &ccirc;ion eblas rigardi hejme
        <fnt>
          <bib>Monato</bib>
          <aut>Dmitrij Cibulevskij</aut>
          <vrk><url ref="&Monato;/2007/009112.php">Busvoja&gcirc;o</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi lastatempe ricevis <nom>Macbook</nom>-on de mia laborejo, kaj &gcirc;i
        inkluzivas kameraon por <tld/>obabili
        <klr>(vd <ref cel="kamera.vide0o">videkamerao</ref>)</klr>
        <fnt>
          <aut>&Gcirc;eremio</aut>,
          <vrk><url ref="http://esperanto-usa.org/eo/node/478">
          Cxu iu volas video-babili?</url></vrk>,
          <lok>[2006]</lok>
        </fnt>;
      </ekz>
      <ekz>
        per <ref cel="konfer.video0o">videokonferenco</ref>
        parolis el Usono profesoro <nom>Sami Repishti</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2011/010506.php">Murdoj memorigitaj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Videotechnik</trd>
  </snc>
  <snc mrk="vide.0o.KIN_videajxo">
    <uzo tip="fak">KIN</uzo>
    <dif>
      <ref tip="dif" cel="vide.0ajxo.KIN">Videa&jcirc;o</ref>:
      <ekz>
        kiel rapide &ccirc;e vi rul<klr>[i&gcirc;]</klr>as <tld/>oj en <nom>YouTube</nom>
        <fnt>
          <aut>Katjo</aut>,
          <vrk><url ref="http://blogo.ikso.net/2010/02/kiel-rapida-estas-via-konektigxo-al-youtube-testu-gxin/">
          Kiel rapida estas via konekti&gcirc;o al YouTube?</url></vrk>,
          <lok>E@i, 2010-02-15</lok>
        </fnt>?
      </ekz>
      <ekz>
        a&ubreve;dregistra&jcirc;oj, <tld/>ofragmentoj kaj cita&jcirc;oj de samtempuloj
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2011/010480.php">Verda vojo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Video</trd>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_d;&c_e;&c_a;</trd>
  <trd lng="cs">video</trd>
  <trd lng="en">video<klr> (n)</klr></trd>
  <trdgrp lng="es">
    <trd>v&iacute;deo</trd>,
    <trd>video</trd>
  </trdgrp>
