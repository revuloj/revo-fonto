<drv mrk="vide.muzik0o">
  <kap>muzik<tld/>o</kap>
  <snc mrk="vide.muzik0o.MUZ">
    <uzo tip="fak">KIN</uzo>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <tld lit="V"/>o<sncref ref="vide.0o.KIN_videajxo"/>
      prezentanta muzika&jcirc;on kun akompana filmo:
      <ekz>
        estis aldonita muzik<tld/>o de la kanto
        <ctl>Berlino sen vi</ctl>
        <fnt>
          <aut>Vinilkosmo</aut>,
          <vrk><url ref="http://www.vinilkosmo.com/?disc=vkkd103">
