  <rad>plutokrati</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="plutok1.0o">
  <kap><tld/>o</kap>
  <snc mrk="plutok1.0o.POL">
    <uzo tip="fak">EKON</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Politika <ref tip="super" cel="regxim.0o.POL">re&gcirc;imo</ref>,
      en kiu regas ri&ccirc;uloj:
      <ekz>
        meze de <tld/>o, komercistoj estas tiom ruzaj, ke ili fari&gcirc;as pli elektemaj ol siaj klientoj
        <fnt>
          <aut>G. K. Chesterton, trad. C. Bean</aut>
          <vrk>La Naiveco de Pastro Brown</vrk>
          <lok>1937</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ju;&c_t;&c_a;&c_k;&c_r;&c_a;&c_t;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_p;&c_l;&c_u;&c_t;&c_o;&c_k;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trd lng="ca">plutocr&agrave;cia</trd>
  <trdgrp lng="cs">
    <trd>plutokracie</trd>,
    <trd>vl&aacute;da bohat&yacute;ch</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Geldherrschaft</trd>,
     <trd>Plutokratie</trd>
  </trdgrp>
  <trd lng="en">plutocracy</trd>
  <trdgrp lng="fa">
    <trd>&f_p;&a_w;&a_l;&#x200C;&a_s;&a_A;&a_l;&a_A;&a_r;&f_y;</trd>,
    <trd>&a_t;&a_w;&a_A;&a_n;&f_g;&a_r;&a_s;&a_A;&a_l;&a_A;&a_r;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">ploutocratie</trd>
  <trdgrp lng="hu">
    <trd>plutokr&aacute;cia</trd>,
    <trd>p&eacute;nz uralma</trd>
  </trdgrp>
  <trd lng="ja">&#x91D1;&#x6A29;&#x653F;&#x6CBB;  <pr>&#x304D;&#x3093;&#x3051;&#x3093;&#x305B;&#x3044;&#x3058;</pr></trd>
  <trd lng="nl">plutocratie</trd>
  <trd lng="pl">plutokracja</trd>
  <trd lng="pt">plutocracia</trd>
  <trd lng="ro">plutocra&#x21B;ie</trd>
  <trd lng="ru">&c_p;&c_l;&c_u;&c_t;&c_o;&c_k;&c_r;&c_a;&c_t;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>plutokracia</trd>,
    <trd>vl&aacute;da bohat&yacute;ch</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x43B;&c_u;&c_t;&#x43E;&#x43A;&c_r;&c_a;&c_t;&c_ib;&#x44F;</trd>
