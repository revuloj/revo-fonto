<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>Paraklet</rad>/o
</kap>
<drv mrk="parakl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KRI</uzo>
  <snc>
    <dif>
      Advokato, defendanto a&ubreve; konsolanto &dash; greka vorto
      trifoje uzita en Evangelio (&Joh; 14:16; 15:26; 16:7) en la
      senco kiun la kristanoj interpretas kiel referencon al la
      <ref tip="sin" cel="spirit.sankta0o">Sankta Spirito</ref>,
      kaj kiel referencon al Kristo en &Jo1; 2:1 (&ccirc;i-loke tradukita
      per <ctl>Advokato</ctl> en la Londona Biblio).
    </dif>
  </snc>
  <trd lng="de">Paraklet</trd>
  <trdgrp lng="en">
    <trd>Holy Spirit</trd>,
    <trd>Paraclete</trd>
  </trdgrp>
  <trd
  lng="grc">&pi;&alfa;&rho;&alfa_acute;&kappa;&lambda;&eta;&tau;&omikron;&sigma_fina;</trd>
  <trdgrp lng="hu">
    <trd>Parakletosz</trd>,
    <trd>k&ouml;zbenj&aacute;r&oacute;</trd>,
    <trd>v&iacute;gasztal&oacute;</trd>
  </trdgrp>
  <trd lng="nl">Paracletus</trd>
  <trdgrp lng="pt">
    <trd>Paracleto</trd>,
    <trd>Esp&iacute;rito Santo</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_U;&c_t;&c_je;&c_sh;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_P;&c_a;&c_r;&c_a;&c_k;&c_l;&c_i;&c_t;</trd>
  </trdgrp>
</drv>
</art>
