<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: irid.xml,v 1.29 2023/11/19 10:22:14 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>irid</rad>/o
</kap>
<drv mrk="irid.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref>
      el familio <ref tip="malprt" cel="iridac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Iris</trd>), kun grandaj trinombraj floroj:
      <ekz>
        en la egiptaj piramidoj trovi&gcirc;as bildoj de <tld/>oj, kiuj dati&gcirc;as de 1500 a.K. 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=200896">Irido</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la gaela vorto <nac>Feileastram</nac> signifas &leftquot;<tld/>o&#x201D;
        <fnt>
          <bib>Monato</bib>
          iridoj<aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2014/010918.php">Mirakloj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="&WCU;/thumb/8/84/Sibirische_Schwertlilie%2C_Iris_sibirica_03.JPG/512px-Sibirische_Schwertlilie%2C_Iris_sibirica_03.JPG" prm="CC BY-SA 3.0 at">siberia <tld/>o (Iris sibirica), Nofels, A&ubreve;strujo
       <fnt>
         <aut>Friedrich B&ouml;hringer (rufre@lenz-nenning.at), propra verko</aut>
         <lok><url ref="&WCW;/File:Sibirische_Schwertlilie,_Iris_sibirica_03.JPG">&WCW;/File:Sibirische_Schwertlilie,_Iris_sibirica_03.JPG</url></lok>
       </fnt>
  </bld>
  <trd lng="be">&c_k;&c_a;&c_s;&c_a;&c_ch; <klr tip="amb">(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trdgrp lng="de">
    <trd>Schwertlilie</trd>,
    <trd>Iris</trd>
  </trdgrp>
  <trd lng="es">iris <klr>(planta)</klr></trd>
  <trd lng="en">iris <klr tip="amb">(flower)</klr></trd>
  <trd lng="fa">&a_z;&a_n;&a_b;&a_q;</trd>
  <trd lng="fr">iris<klr tip="ind"> (plante)</klr></trd>
  <trdgrp lng="hu">
    <trd>n&odblac;szirom</trd>,
    <trd>&iacute;risz</trd></trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30A2;&#x30E4;&#x30E1;</trd>,
    <trd>&#x30A2;&#x30A4;&#x30EA;&#x30B9;</trd>
  </trdgrp>
  <trd lng="nl">iris<klr tip="ind"> (bloem)</klr></trd>
  <trdgrp lng="pl">
    <trd>kosaciec</trd>,
    <trd>irys</trd>
  </trdgrp>
  <trd lng="pt">&iacute;ris <klr tip="amb">(bot.)</klr></trd>
  <trd lng="ru">&c_i;&c_r;&c_i;&c_s;</trd>
  <trd lng="sk">kosatec</trd>
  <trd lng="cs">kosatec</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x456;&#x432;&#x43D;&#x438;&#x43A;&#x438;</trd>,
    <trd>&#x456;&#x440;&#x438;&#x441;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8774;&#x8776;&#x82B1; <pr>h&#xFA;di&#xE9;hu&#x101;</pr></trd>,
    <trd>&#x8679;&#x5F69; <pr>h&#xF3;ngc&#x1CE;i</pr></trd>,
    <trd>&#x99AC;&#x862D;&#x82B1; <pr>m&#x1CE;l&#xE1;nhu&#x101;</pr></trd>
  </trdgrp>
</drv>

</art>
