  <trd lng="fr">iris<klr tip="ind"> (plante)</klr></trd>
  <trdgrp lng="hu">
    <trd>n&odblac;szirom</trd>,
    <trd>&iacute;risz</trd></trdgrp>
  <trd lng="nl">iris<klr tip="ind"> (bloem)</klr></trd>
  <trd lng="pt">&iacute;ris <klr tip="amb">(bot.)</klr></trd>
  <trd lng="ru">&c_i;&c_r;&c_i;&c_s;</trd>
</drv>

