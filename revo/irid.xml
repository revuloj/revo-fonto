<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>irid</rad>/o
</kap>
<drv mrk="irid.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="irid.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Iris</trd>), kun grandaj trinombraj floroj.
    </dif>
  </snc>
<bld lok="../bld/irid.jpg"></bld>
  <trd lng="de">Schwertlilie</trd>
</drv>
<drv mrk="irid.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.unu0aj">unukotiledonaj</ref>
      plantoj (<trd lng="la">Iridaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Schwertliliengew&auml;chse</trd>
</drv>
</art>
