<vortaro>
  <art mrk="$Id: biskot.xml,v 1.2 2024/04/03 06:28:36 afido Exp $">
    <kap>
      <rad>biskot</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="biskot.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="sin" cel="biskvi.0o.dubakita">Biskvito<sncref/></ref>, dubakita pantran&ccirc;o:
      <ekz>
        estis pakitaj fuma&jcirc;ita viando, fi&scirc;o, sekalbrando, <tld/>o kaj kaviarskatoloj,
        tekru&ccirc;o en fajrofara kesteto
        <fnt>
          <aut>M&oacute;r J&oacute;kai, trad. L&aacute;szl&oacute; P&aacute;sztor	</aut>
          <vrk><url
          ref="http://www.verkoj.com/tradukistoj/laszlo-pasztor/la-filoj-de-la-sxtonkora-homo/"
              >La filoj de l&#8217; &scirc;tonkora homo</url></vrk>
          <lok>Du bonaj amikoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj &scirc;ategas kukon surbaze de <tld/>o kovrita per frukta gelatena&jcirc;o
        <fnt>
          <vrk><url
          ref="http://esperantaretradio.blogspot.com/2015/06/donaco-de-dio-el-kasubio.html"
              >Donaco de Dio el Ka&scirc;ubio</url></vrk>
          <lok>Esperanta Retradio, 2015-06-25</lok>
        </fnt>;
      </ekz>
      <ekz>
        oran&gcirc;migdala <tld/>o
        <fnt>
          <vrk><url ref="https://nask.esperanto-usa.org/jxurnalo/2008/pdf/fasko-08.pdf"
              >Baki vegane?</url></vrk>
          <lok>la naska fasko, n-ro 8, 2008-07-09, p-3</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Zwieback</trd>
    <trd lng="en">zwieback</trd>
    <trd lng="fr">biscotte</trd>
  </snc>
<trd lng="be">&c_s;&c_u;&c_h;&c_a;&c_r;</trd>
</drv>

  </art>

