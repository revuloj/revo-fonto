<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ampol.xml,v 1.33 2024/01/11 10:33:05 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>ampol</rad>/o <fnt><aut>K. Bennemann</aut>, <vrk>Esperanto
  Hand-W&ouml;rterbuch</vrk>, <lok>1923-1926</lok></fnt>
</kap>
<drv mrk="ampol.0o">
  <kap><tld/>o</kap>
  <snc mrk="ampol.0o.boteleto">
    <dif>
      Vitra <ref tip="super" cel="botel.0o.VAZ">boteleto</ref> por
      konservi likvan kuracilon, &hcirc;emia&jcirc;on ktp:
      <ekz>
        <tld/>o kun likva kaj vapora bromo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=22392">Bromo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_m;&c_p;&c_u;&c_l;&c_a;</trd>,
      <trd>&c_k;&c_o;&c_l;&c_b;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Ampulle</trd>,
      <trd>Fl&auml;schchen</trd>
    </trdgrp>
    <trd lng="en">bulb</trd>
    <trd lng="es">ampolla</trd>
    <trd lng="fa">&a_A_madda;&a_m;&f_p;&a_w;&a_l;<klr tip="ind"> (&a_sh;&f_y;&a_sh;&a_h;)</klr></trd>
    <trdgrp lng="fr">
      <trd>ampoule<klr tip="ind"> (fiole)</klr></trd>,
      <trd>fiole<klr tip="ind"> (ventrue)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>retorta</trd>,
      <trd>g&ouml;reb</trd>
    </trdgrp>
    <trd lng="nl">ampul</trd>
    <trdgrp lng="pl">
      <trd>ampu&lstroke;ka</trd>,
      <trd>kolba</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_a;&c_m;&c_p;&c_u;&c_l;&c_a;</trd>,
      <trd>&c_k;&c_o;&c_l;&c_b;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x5B89;&#x74FF; <pr>&amacron;npou</pr></trd>,
    <trd>&#x5B89;&#x74FF;&#x74F6; <pr>&amacron;nb&#xF9;p&#xED;ng</pr></trd>,
    <trd>&#x4EFB;&#x4F55;&#x74F6;&#x72B6;&#x5BB9;&#22120;
    <pr>r&#xE8;nh&#xE9;p&#xED;ngzhu&#xE0;ngr&#xF3;ngq&#xEC;</pr></trd>,
    <trd>&#x4EFB;&#x4F55;&#x74F6;&#x72C0;&#x5BB9;&#22120;
    <pr>r&#xE8;nh&#xE9;p&#xED;ngzhu&#xE0;ngr&#xF3;ngq&#xEC;</pr></trd>,
    <trd>&#x5C0F;&#x74F6; <pr>xi&#x1CE;op&#xED;ng</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="ampol.0o.elektra">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Vitra ujo kun interna <ref tip="prt" cel="filame.0o.ELE">filamento</ref>,
      produktanta lumon, kiam &gcirc;in trairas kurento:
      <ekz>
        <nom>Edison</nom> ne estis la unua inventinto de la <tld/>o anka&ubreve;
        nom<klr>[a]</klr>ta inkandeska lampo, diversaj aliaj anta&ubreve;e laboris pri tio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=991">Thomas Alva Edison</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="lamp.0o">lampo</ref>
    <trd lng="be">&c_l;&c_ja;&c_m;&c_p;&c_a;&c_ch;&c_k;&c_a; <klr>(&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_ja;)</klr></trd>
    <trdgrp lng="de">
      <trd>Gl&uuml;hbirne</trd>,
      <trd><klr>(Gl&uuml;h-)</klr>Birne </trd>,
      <trd>Gl&uuml;hlampe</trd>
    </trdgrp>
    <trd lng="en">(light) <ind>bulb</ind></trd>
    <trd lng="es">bombilla</trd>
    <trd lng="fa">&a_l;&a_A;&a_m;&f_p;</trd>
    <trd lng="fr">ampoule<klr tip="ind"> (&eacute;lectrique)</klr></trd>
    <trdgrp lng="hu">
      <trd>villanyk&ouml;rte</trd>,
      <trd>izz&oacute;</trd>,
      <trd>villany&eacute;g&odblac;</trd>
    </trdgrp>
    <trd lng="nl">gloeilamp</trd>
    <trd lng="pl">&zdot;ar&oacute;wka</trd>
    <trd lng="ru">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja; <ind>&c_l;&c_a;&c_m;&c_p;&c_o;&c_ch;&c_k;&c_a;</ind></trd>
    <trdgrp lng="zh">
    <trd>&#30005;&#x706F;&#x6CE1; <pr>di&#xE0;nd&emacron;ngp&#xE0;o</pr></trd>,
    <trd>&#x96FB;&#x71C8;&#x6CE1; <pr>di&#xE0;nd&emacron;ngp&#xE0;o</pr></trd>,
    <trd>&#x706F;&#x6CE1; <pr>d&emacron;ngp&#xE0;o</pr></trd>,
    <trd>&#x71C8;&#x6CE1; <pr>d&emacron;ngp&#xE0;o</pr></trd>,
    <trd>&#x767D;&#x70BD;&#x706F; <pr>b&#xE1;ich&#xEC;d&emacron;ng</pr></trd>,
    <trd>&#x767D;&#x71BE;&#x71C8; <pr>b&#xE1;ich&#xEC;d&emacron;ng</pr></trd>,
    <trd>&#x767D;&#x70BD;&#x706F;&#x6CE1; <pr>b&#xE1;ich&#xEC;d&emacron;ngp&#xE0;o</pr></trd>,
    <trd>&#x767D;&#x71BE;&#x71C8;&#x6CE1; <pr>b&#xE1;ich&#xEC;d&emacron;ngp&#xE0;o</pr></trd>
  </trdgrp>
</snc>
  <trd lng="bo">&#xF64;&#xF7A;&#xF63;&#xF0B;&#xF4F;&#xF7C;&#xF42;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>ampule</trd>,
    <trd>ampulka</trd>,
    <trd>ba&ncaron;at&aacute; l&aacute;hev</trd>,
    <trd>roz&scaron;&iacute;&rcaron;en&iacute; trubice</trd>,
    <trd>&zcaron;&aacute;rovka</trd>
  </trdgrp>
  <trd lng="he">&#x5D0;&#x5B7;&#x5DE;&#x5E4;&#x5BC;&#x5D5;&#x5BC;&#x5DC;&#x5B8;&#x5D4;</trd>
  <trdgrp lng="ja">
    <trd>&#x30A2;&#x30F3;&#x30D7;&#x30EB;</trd>,
    <trd>&#x96FB;&#29699;  <pr>&#12391;&#12435;&#x304D;&#12421;&#12358;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>1.&zcaron;iarovka</trd>,
    <trd>2.elektronka (el. tech.)</trd>,
    <trd>banka <klr>(n&aacute;doba)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&#x43C;&#x43F;&c_u;&#x43B;&c_a;</trd>,
    <trd>&#x43A;&#x43E;&#x43B;&c_b;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="ampol.0ingo">
  <kap><tld/>ingo <fnt><bib>PIV2</bib></fnt></kap>
  <snc mrk="ampol.0ingo.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Organo de elektra cirkvito, destinita por ricevi <tld/>on kaj
      transdoni al &gcirc;i la kurenton:
      <ekz>
        <nom>Aleksander Lodigin</nom>: <klr>[&#8230;]</klr>
        en la praktika uzo estis grava &gcirc;is hodia&ubreve; lia tipo de <tld/>ingo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=374142">Aleksander Lodigin</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="lamp.0ingo">lampingo</ref>
    <trd lng="de"><klr>(Lampen-)</klr>Fassung</trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_t;&c_r;&c_o;&c_n; <klr tip='amb'>(&c_l;&c_ja;&c_m;&c_p;&c_a;&c_ch;&c_k;&c_ib;)</klr></trd>
  <trd lng="cs">obj&iacute;mka na &zcaron;&aacute;rovku</trd>
  <trd lng="es">casquillo</trd>
  <trd lng="fa">&a_s;&a_r;&f_p;&f_y;&f_ch;</trd>
  <trd lng="fr">douille<klr tip="ind"> (d'ampoule)</klr></trd>
  <trd lng="hu">foglalat<klr tip="ind"> (l&aacute;mp&aacute;&eacute;)</klr></trd>
  <trdgrp lng="pl">
  <trd>oprawa <klr tip="amb"/>&zdot;ar&oacute;wki</trd>,
  <trd>gniazdo <klr tip="amb">&zdot;ar&oacute;wki</klr></trd>
</trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_t;&c_r;&c_o;&c_n;<klr tip="amb"> (&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_j;
  &c_l;&c_a;&c_m;&c_p;&c_o;&c_ch;&c_k;&c_i;)</klr></trd>
  <trd lng="sk">obj&iacute;mka &zcaron;iarovky</trd>
</drv>

</art>
