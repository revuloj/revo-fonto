<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>ampol</rad>/o
  <fnt>N</fnt>
</kap>
<drv mrk="ampol.0o">
  <kap><tld/>o</kap>
  <snc mrk="ampol.0o.boteleto">
    <dif>
      Ventroforma boteleto.
    </dif>
    <trdgrp lng="de">
      <trd>Bauchflasche</trd>
    </trdgrp>
    <trd lng="en">bulb</trd>
    <trdgrp lng="fr">
      <trd>ampoule</trd>,
      <trd>fiole <klr>(ventrue)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>retorta</trd>,
      <trd>g&ouml;reb</trd>
    </trdgrp>
    <trd lng="nl">ampul</trd>
    <trdgrp lng="ru">
      <trd>&c_a;&c_m;&c_p;&c_u;&c_l;&c_a;</trd>,
      <trd>&c_k;&c_o;&c_l;&c_b;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="ampol.0o.lampo">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Elektra lampo.
    </dif>
    <trdgrp lng="de">
      <trd>Gl&uuml;hbirne</trd>,
      <trd>Birne</trd>,
      <trd>Leuchte</trd>
    </trdgrp>
    <trd lng="en">(electric) <ind>lamp</ind></trd>
    <trd lng="fr">ampoule (&eacute;lectrique)</trd>
    <trdgrp lng="hu">
      <trd>villanyk&ouml;rte</trd>,
      <trd>izz&oacute;</trd>,
      <trd>villany&eacute;g&odblac;</trd>
    </trdgrp>
    <trd lng="nl">gloeilamp</trd>
    <trd lng="ru">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja; &c_l;&c_a;&c_m;&c_p;&c_o;&c_ch;&c_k;&c_a;</trd>
  </snc>
</drv>
</art>
