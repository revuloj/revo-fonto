<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>ampol</rad>/o
  <fnt>N</fnt>
</kap>
<drv mrk="ampol.0o">
  <kap><tld/>o</kap>
  <snc mrk="ampol.0o.boteleto">
    <dif>
      Ventroforma boteleto.
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_m;&c_p;&c_u;&c_l;&c_a;</trd>,
      <trd>&c_k;&c_o;&c_l;&c_b;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Bauchflasche</trd>
    </trdgrp>
    <trd lng="en">bulb</trd>
    <trd lng="es">ampolla</trd>
    <trdgrp lng="fr">
      <trd>ampoule<klr tip="ind"> (fiole)</klr></trd>,
      <trd>fiole<klr tip="ind"> (ventrue)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>retorta</trd>,
      <trd>g&ouml;reb</trd>
    </trdgrp>
    <trd lng="nl">ampul</trd>
    <trdgrp lng="ru">
      <trd>&c_a;&c_m;&c_p;&c_u;&c_l;&c_a;</trd>,
      <trd>&c_k;&c_o;&c_l;&c_b;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="ampol.0o.elektra">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Vitra ujo kun interna <ref tip="prt" cel="filame.0o.ELE">filamento</ref>,
      produktanta lumon, kiam &gcirc;in trairas kurento.
    </dif>
    <trd lng="be">&c_l;&c_ja;&c_m;&c_p;&c_a;&c_ch;&c_k;&c_a;
<klr>(&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_ja;)</klr></trd>
    <trdgrp lng="de">
      <trd>Gl&uuml;hbirne</trd>,
      <trd>Birne</trd>,
      <trd>Leuchte</trd>
    </trdgrp>
    <trd lng="en">(light) <ind>bulb</ind></trd>
    <trd lng="es">bombilla</trd>
    <trd lng="fr">ampoule<klr tip="ind"> (&eacute;lectrique)</klr></trd>
    <trdgrp lng="hu">
      <trd>villanyk&ouml;rte</trd>,
      <trd>izz&oacute;</trd>,
      <trd>villany&eacute;g&odblac;</trd>
    </trdgrp>
    <trd lng="nl">gloeilamp</trd>
    <trd lng="ru">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja;
<ind>&c_l;&c_a;&c_m;&c_p;&c_o;&c_ch;&c_k;&c_a;</ind></trd>
  </snc>
</drv>

<drv mrk="ampol.0ingo">
  <kap><tld/>ingo</kap>
  <uzo tip="fak">ELE</uzo>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <dif>
      Organo de elektra cirkvito, destinita por ricevi <tld/>on kaj
      transdoni al &gcirc;i la kurenton.
    </dif>
  </snc>
  <trd lng="es">casquillo</trd>
  <trd lng="fr">douille<klr tip="ind"> (d'ampoule)</klr></trd>
  <trd lng="hu">foglalat<klr tip="ind">
(l&aacute;mp&aacute;&eacute;)</klr></trd>
  <trd lng="ru">&c_p;&c_a;&c_t;&c_r;&c_o;&c_n;<klr tip="amb">
(&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_j;
&c_l;&c_a;&c_m;&c_p;&c_o;&c_ch;&c_k;&c_i;)</klr></trd>
</drv>

</art>
