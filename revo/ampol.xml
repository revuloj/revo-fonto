<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>ampol</rad>/o <fnt><aut>K. Bennemann</aut>, <vrk>Esperanto
  Hand-W&ouml;rterbuch</vrk>, <lok>1923-1926</lok></fnt>
   <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ampol.0o">
  <kap><tld/>o</kap>
  <snc mrk="ampol.0o.boteleto">
    <dif>
      Ventroforma <ref tip="super" cel="botel.0o.VAZ">boteleto</ref>.
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_m;&c_p;&c_u;&c_l;&c_a;</trd>,
      <trd>&c_k;&c_o;&c_l;&c_b;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Bauchflasche</trd>
    </trdgrp>
    <trd lng="en">bulb</trd>
    <trd lng="es">ampolla</trd>
    <trd lng="fa">&a_A_madda;&a_m;&f_p;&a_w;&a_l;<klr tip="ind"> (&a_sh;&f_y;&a_sh;&a_h;)</klr></trd>
    <trdgrp lng="fr">
      <trd>ampoule<klr tip="ind"> (fiole)</klr></trd>,
      <trd>fiole<klr tip="ind"> (ventrue)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>retorta</trd>,
      <trd>g&ouml;reb</trd>
    </trdgrp>
    <trd lng="nl">ampul</trd>
    <trdgrp lng="pl">
      <trd>ampu&lstroke;ka</trd>,
      <trd>kolba</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_a;&c_m;&c_p;&c_u;&c_l;&c_a;</trd>,
      <trd>&c_k;&c_o;&c_l;&c_b;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="ampol.0o.elektra">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Vitra ujo kun interna <ref tip="prt"
      cel="filame.0o.ELE">filamento</ref>,
      produktanta lumon, kiam &gcirc;in trairas kurento.
    </dif>
    <trd lng="be">&c_l;&c_ja;&c_m;&c_p;&c_a;&c_ch;&c_k;&c_a; <klr>(&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_ja;)</klr></trd>
    <trdgrp lng="de">
      <trd>Gl&uuml;hbirne</trd>,
      <trd>Birne</trd>,
      <trd>Leuchte</trd>
    </trdgrp>
    <trd lng="en">(light) <ind>bulb</ind></trd>
    <trd lng="es">bombilla</trd>
    <trd lng="fa">&a_l;&a_A;&a_m;&f_p;</trd>
    <trd lng="fr">ampoule<klr tip="ind"> (&eacute;lectrique)</klr></trd>
    <trdgrp lng="hu">
      <trd>villanyk&ouml;rte</trd>,
      <trd>izz&oacute;</trd>,
      <trd>villany&eacute;g&odblac;</trd>
    </trdgrp>
    <trd lng="nl">gloeilamp</trd>
    <trd lng="pl">&zdot;ar&oacute;wka</trd>
    <trd lng="ru">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja; <ind>&c_l;&c_a;&c_m;&c_p;&c_o;&c_ch;&c_k;&c_a;</ind></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>ampule</trd>,
    <trd>ampulka</trd>,
    <trd>ba&ncaron;at&aacute; l&aacute;hev</trd>,
    <trd>roz&scaron;&iacute;&rcaron;en&iacute; trubice</trd>,
    <trd>&zcaron;&aacute;rovka</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>1.&zcaron;iarovka</trd>,
    <trd>2.elektronka (el. tech.)</trd>,
    <trd>banka <klr>(n&aacute;doba)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="ampol.0ingo">
  <kap><tld/>ingo <fnt><bib>PIV2</bib></fnt></kap>
  <snc mrk="ampol.0ingo.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Organo de elektra cirkvito, destinita por ricevi <tld/>on kaj
      transdoni al &gcirc;i la kurenton.
    </dif>
  </snc>
  <trd lng="cs">obj&iacute;mka na &zcaron;&aacute;rovku</trd>
  <trd lng="es">casquillo</trd>
  <trd lng="fa">&a_s;&a_r;&f_p;&f_y;&f_ch;</trd>
  <trd lng="fr">douille<klr tip="ind"> (d'ampoule)</klr></trd>
  <trd lng="hu">foglalat<klr tip="ind"> (l&aacute;mp&aacute;&eacute;)</klr></trd>
  <trdgrp lng="pl">
  <trd>oprawa <klr tip="amb"></klr>&zdot;ar&oacute;wki</trd>,
  <trd>gniazdo <klr tip="amb">&zdot;ar&oacute;wki</klr></trd>
</trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_t;&c_r;&c_o;&c_n;<klr tip="amb"> (&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_j;
  &c_l;&c_a;&c_m;&c_p;&c_o;&c_ch;&c_k;&c_i;)</klr></trd>
  <trd lng="sk">obj&iacute;mka &zcaron;iarovky</trd>
</drv>

</art>
