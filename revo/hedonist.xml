<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hedonist.xml,v 1.8 2022/06/09 08:40:57 revo Exp $">
<kap>
  <rad>hedonist</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="hedonist.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="hedonist.0o.FIL">
    <dif>
      <ref tip="vid" cel="hedonism.0o.FIL">Hedonism</ref>ano,
      &gcirc;uamanto:
      <ekz>
        <tld/>o <nom>Pomet</nom> <klr>[...]</klr> &ccirc;i-maniere
        parolas pri man&gcirc;a&jcirc;oj anta&ubreve; si<klr>...</klr>
        <fnt>
          <aut>Z. Sele&scaron;</aut>,
          <vrk><url
          ref="http://www.gimnazija-ikranjceva-djurdjevac.skole.hr/upload/gimnazija-ikranjceva-djurdjevac/newsattach/549/ONKLO_MAROJE.docx">
          Onklo Maroje...</url></vrk>,
          <lok>Literatura foiro, 2010-06 (245)</lok>
        </fnt>;
      </ekz>
      <ekz>
        malgra&ubreve; sia aspekto kiel ri&ccirc;a <tld/>o <klr>[li]</klr> emas montri siajn piecon
        kaj socian konservativismon
        <fnt>
          <bib>MD17</bib>
          <vrk>Imran Khan, de kriketo al registarestro</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="viv.bon0anto">bonvivanto</ref>,
      <ref cel="epikur.e0ano.KOMUNE">epikurano<sncref/></ref>,
      <ref cel="gxu.0emulo">&gcirc;uemulo</ref>,
      <ref cel="sibari.0o.MOR">sibarito</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_d;&c_a;&c_n;&c_ib;&c_s;&c_t;</trd>
  <trd lng="ca">hedonista<klr> (subst.)</klr></trd>
  <trd lng="cs">h&eacute;donista</trd>
  <trd lng="de">Hedonist</trd>
  <trd lng="fr">h&eacute;doniste<klr> (subst.)</klr></trd>
  <trd lng="sk">hedonista</trd>
  <trd lng="uk">&c_g;&c_je;&c_d;&#x43E;&#x43D;&c_ib;&c_s;&c_t;</trd>
</drv>
</art>

