<vortaro>
  <art mrk="$Id: bulimi.xml,v 1.6 2023/11/12 15:25:19 afido Exp $">
    <kap>
      <rad>bulimi</rad>/o
    </kap>

<drv mrk="bulimi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc mrk="bulimi.0o.malsano">
    <dif>
      <ref tip="lst" lst="voko:malsanoj" cel="san.mal0o.MED">Malsano</ref> karakterizita de troa
      apetito:
      <ekz>
        gefiloj povas havi problemojn: <ref cel="anoreksi.0o">anoreksio</ref>, <tld/>o, obezeco
        <fnt>
          <vrk><url ref="http://www.esperanto-sumoo.pl/recenzojan2017.html">Recenzoj: Komunikado en
          amrilato ene de paro</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        anoreksio kaj <tld/>o &dash; &ccirc;u doloroj nur de la 20a jarcento
        <fnt>
          <aut>Dr. Danuta Sadownik</aut>
          <vrk>prelego</vrk>
        </fnt>?
      </ekz>
      <ekz>
        oni trovas &scirc;in tro magra kaj <klr>[&#8230;]</klr> akuzis &scirc;in je <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=129454">Kate Moss</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_u;&c_l;&c_ib;&c_m;&c_ib;&c_ja;</trd>
    <trd lng="de">Bulimie</trd>
    <trd lng="fr">boulimie</trd>
    <trd lng="zh">&#x66B4;&#x98DF;&#x75C7; <pr>b&#xE0;osh&#xED;zh&#xE8;ng</pr></trd>
</snc>
  <trd lng="he">&#x5D1;&#x5D5;&#x5DC;&#x5D9;&#x5DE;&#x5D9;&#x5D4;</trd>
  <trd lng="uk">&c_b;&c_u;&#x43B;&c_ib;&#x43C;&c_ib;&#x44F;</trd>
</drv>

  </art>

