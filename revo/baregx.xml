<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: baregx.xml,v 1.6 2024/09/19 12:51:59 revo Exp $">
<kap>
  <rad>bare&gcirc;</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="baregx.0o">
  <kap><tld/>o</kap>
  <snc mrk="baregx.0o.TEKS">
  <uzo tip="fak">TEKS</uzo>
    <dif>
      Tre malpeza lana <ref tip="super" cel="sxtof.0o.TEKS">&scirc;tofo</ref>
      ekproduktita en la 18a jarcento en samnoma franca urbo:
      <ekz>
        &scirc;i portis malpezan <tld/>an veston
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 16a, p.
        84a</lok></fnt>.
      </ekz>
    </dif>

    <trdgrp lng="de">
      <trd>Barege</trd>,
      <trd>Barrege</trd>
    </trdgrp>
    <trd lng="fr">bar&egrave;ge</trd>
    <trd lng="pl">bare&zdot;</trd>
    <trd lng="pt">barege</trd>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_r;&c_e;&c_zh; <klr>(&c_t;&c_k;&c_a;&c_n;&c_ib;&c_n;&c_a;)</klr></trd>
  <trd lng="uk">&c_b;&c_a;&c_r;&c_je;&c_zh;</trd>
</drv>
</art>

