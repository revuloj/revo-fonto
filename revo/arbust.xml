<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>6</ofc>
<rad>arbust</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="arbust.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Ligna, <ref tip="super" cel="plant.dauxra0o">da&ubreve;ra planto</ref>,
      disbran&ccirc;i&gcirc;anta &ccirc;e la bazo, sed
      pli malgranda ol <ref tip="vid" cel="arbed.0o">arbedo</ref>:
      <ekz>
        kaprifolio, berberiso, rubuso, eriko estas <tld/>oj;
      </ekz>
      <ekz>
        en &gcirc;iaj palacoj kreskos dornoj kaj urtikoj kaj sur
        &gcirc;iaj fortika&jcirc;oj kreskos pikarbustoj
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Jesaja 34, 13</lok>)</fnt>;
      </ekz>
      <ekz>
        se ne tiam eliros fajro el la dornarbusto kaj forbruligos
        la cedrojn de Lebanon
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Ju&gcirc;istoj 9, 15</lok>)</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_u;&c_s;&c_t;</trd>, 
    <trd>&c_k;&c_u;&c_s;&c_t;&c_o;&c_w;&c_je;</trd>, 
    <trd>&c_h;&c_m;&c_y;&c_z;&c_n;&c_ja;&c_k;</trd>
  </trdgrp>  
  <trdgrp lng="de">
    <trd><klr tip="ind">(kleiner)</klr> Strauch</trd>, 
    <trd>Zwergstrauch <klr>(rekta)</klr></trd>,
    <trd>Spalierstrauch <klr>(ku&scirc;anta)</klr></trd>
  </trdgrp>
  <trd lng="en">shrub</trd>
  <trd lng="fr">arbuste</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_s;&c_t;</trd>,
    <trd>&c_k;&c_u;&c_s;&c_t;&c_a;&c_r;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
