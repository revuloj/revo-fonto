<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>trog</rad>/o
</kap>
<drv mrk="trog.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Granda longforma ujo, en kiu oni donas man&gcirc;a&jcirc;ojn a&ubreve; trinka&jcirc;on al
      bestoj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_r;&c_y;&c_t;&c_a;</trd>, 
    <trd>&c_k;&c_a;&c_r;&c_m;&c_u;&c_sh;&c_k;&c_a;</trd>
  </trdgrp>  
  <trd lng="de">Trog</trd>
  <trd lng="nl">trog</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_r;&c_y;&c_t;&c_o;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_m;&c_u;&c_sh;&c_k;&c_a; <klr tip="amb">(&c_d;&c_l;&c_ja; &c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_y;&c_h;)</klr></trd>
  </trdgrp>
</drv>
</art>
