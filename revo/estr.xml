  <trd lng="be">&c_p;&c_r;&c_y;&c_z;&c_n;&c_a;&c_ch;&c_y;&c_c;&c_mol; <ind>&c_k;&c_ib;&c_r;&c_a;&c_w;&c_n;&c_ib;&c_k;&c_o;&c_m;</ind></trd>
