<drv mrk="estr.0igi">
  <kap>
    <tld/>igi
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="posten.en0igi">Enpostenigi</ref> kiel <tld/>o:
      <ekz>
        li <tld/>igis lin super sia domo, kaj transdonis en liajn manojn &ccirc;ion, kion li
        havis
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 39:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        kial do vi <tld/>igas vin super la komunumo <klr>[&#8230;]</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 16:3</lok>
        </fnt>?
      </ekz>
      <ekz>
        kiun mi <tld/>igu super ili? &ndash; <tld/>igu min, sinjoro &ndash; diris <nom>Domitius</nom>
        <fnt>
          <bib>QuV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="avanc.0i">avanci</ref>gi,
      <ref cel="ofic.en0igi">enoficigi</ref>,
