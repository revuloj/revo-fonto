<drv mrk="estr.0arano">
  <kap><tld/>arano</kap>
  <snc>
    <dif>
      Membro de <tld/>aro:
      <ekz>
        la ministro pri edukado <klr>[estas]</klr> <tld/>arano de la separatisma partio
       <fnt>
       <aut>IRL</aut>
          <vrk><url ref="&Monato;/2000/006826.php">&Ccirc;u pa&scirc;oj al paco?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 7</lok>
        </fnt>;
      </ekz>
      <ekz>
        ja dum pli ol 40 jaroj li okupis &scirc;losilajn poziciojn <klr>[&#8230;]</klr> kiel
        prezidanto a&ubreve; <tld/>arano de TEJO kaj UEA
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="&Monato;/2007/009211.php">Opinioj plej kompetentaj</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        la teknikan funkciigon faris <klr>[li]</klr> kadre de sia agado kiel <tld/>arano
        de Universala Esperanto-Asocio pri informado
        <fnt>
          <bib>Monato</bib>
          <aut>Grigorij Arosev, Andrej Grigorjevskij</aut>
          <vrk><url ref="&Monato;/2004/008210.html">Abonu!</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#8230;]</klr> volas silentigi la kritikon per revizio de la interna regularo
        pri la <tld/>aranoj de Japana Banko
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="&Monato;/2006/009053.php">Prezidanto de la japana centra banko
          profitis</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        estontece 35&#8239;% de la <tld/>aranoj estu virinoj
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2011/010346.php">Koalicia kompromiso</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_l;&c_je;&c_c; &c_r;&c_a;&c_d;&c_y; <ind>&c_d;&c_y;&c_r;&c_e;&c_k;&c_t;&c_a;&c_r;&c_a;&c_w;</ind></trd>
  <trdgrp lng="en">
    <trd>board member</trd>,
    <trd>director</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#24441;&#21729;  <pr>&#12420;&#12367;&#12356;&#12435;</pr></trd>,
    <trd>&#29702;&#20107;  <pr>&#12426;&#12376;</pr></trd>
  </trdgrp>
  <trd lng="nl">bestuurslid</trd>
  <trd lng="uk">&c_ch;&c_l;&c_je;&c_n; &c_p;&c_r;&c_a;&c_v;&c_l;&c_ib;&c_n;&c_n;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#33891;&#20107; <pr>d&#466;ngsh&igrave;</pr></trd>,
    <trd>&#33891;&#20107;&#20250;&#25104;&#21592; <pr>d&#466;ngsh&igrave;hu&igrave;ch&eacute;ngyu&aacute;n</pr></trd>
  </trdgrp>
