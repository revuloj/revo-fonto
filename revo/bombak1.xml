<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bombak1.xml,v 1.8 2019/03/28 19:10:16 revo Exp $">
<kap>
  <rad>bombakac</rad>/oj
</kap>
<drv mrk="bombak1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref>  de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      tropikaj arboj (<trd lng="la">Bombacaceae</trd>):
      <ekz>
        la <ref tip="prt" cel="baobab.0o">baobabo</ref> estas <klr>[&#8230;]</klr>
        genro el <tld/>oj, kiu &gcirc;enerale konstituas tre
        diktrunkajn (kelkfoje 9-metra diametro), vast-bran&ccirc;arajn arbojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=36290"
              >Baobabo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_m;&c_b;&c_a;&c_k;&c_s;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Wollbaumgew&auml;chse</trd>
  <trd lng="fr">bombacac&eacute;es</trd>
  <trdgrp lng="hu">
    <trd>majomkeny&eacute;rfaf&eacute;l&eacute;k</trd>,
    <trd>bombaxf&eacute;l&eacute;k</trd></trdgrp>
  <trd lng="ru">&c_b;&c_o;&c_m;&c_b;&c_a;&c_k;&c_s;&c_o;&c_v;&c_y;&c_je;
<klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>

