<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>&scirc;vel</rad>/i
</kap>
<drv mrk="sxvel.0i">
  <kap>
  <ofc>*</ofc><tld/>i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Plivolumeni&gcirc;i, plidiki&gcirc;i,  plivasti&gcirc;i:
      <ekz>
        lia vango <tld/>as pro absceso;
      </ekz>
      <ekz>
        <tld/>inta kiel balono, kiel bufo;
      </ekz>
      <ekz>
        <tld/>inta rivero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la vento <tld/>igas la velon;
      </ekz>
      <ekz>
        <tld/>mola (mola kaj elasta) tapi&scirc;o;
      </ekz>
      <ekz>
        brusto<tld/>i
        <klr>(brustostre&ccirc;i)</klr>.
      </ekz>
    </dif>
    <trdgrp lng="pt">
      <trd>inchar</trd>,
      <trd>aumentar <klr>(de volume)</klr></trd>,
      <trd>avolumar-se</trd>,
      <trd>crescer</trd>,
      <trd>expandir-se</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Trodiki&gcirc;i, trovasti&gcirc;i:
      <ekz>
        li <tld/>is de fiero<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>parola
        <klr>(bombasta)</klr>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="puf.0igxi">pufi&gcirc;i</ref>
    </refgrp>
    <trdgrp lng="pt">
      <trd>alargar-se</trd>,
      <trd>avolumar-se</trd>,
      <trd>dilatar-se</trd>,
      <trd>expandir-se</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Pligrandi&gcirc;i la&ubreve; iu ajn rilato, plialti&gcirc;i,
      plimulti&gcirc;i, pliforti&gcirc;i:
      <ekz>
        la kantoj grade <tld/>adis;
      </ekz>
      <ekz>
        la fusilbruado <tld/>as;
      </ekz>
      <ekz>
        ni ne tro <tld/>igu nian vortaron.
      </ekz>
    </dif>
    <trdgrp lng="pt">
      <trd>avolumar-se</trd>,
      <trd>inchar</trd>,
      <trd>aumentar</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Tromulti&gcirc;i, trokreski:
      <ekz>
        la novaj merkatoj estas nekapablaj engluti la &ccirc;iam pli
        <tld/>intan
        produktadon de l' industrio;
      </ekz>
      <ekz>
        <tld/>igitaj prezoj.
      </ekz>
    </dif>
    <trdgrp lng="pt">
      <trd>aumentar</trd>,
      <trd>crescer</trd>,
      <trd>expandir-se</trd>
    </trdgrp>
  </snc>
  <refgrp tip="vid">
    <ref cel="strecx.0i">stre&ccirc;i</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_p;&c_u;&c_h;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_p;&c_u;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_b;&c_r;&c_y;&c_n;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_b;&c_r;&c_a;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">schwellen</trd>
  <trd lng="en">swell</trd>
  <trdgrp lng="fr">
    <trd>enfler</trd>,
    <trd>gonfler<klr> (intr.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>duzzad</trd>,
    <trd>dagad</trd>,
    <trd>n&ouml;vekszik<klr tip="ind"> (&aacute;tv.)</klr></trd>
  </trdgrp>
  <trd lng="nl">zwellen</trd>
  <trdgrp lng="pl">
    <trd>puchn&aogonek;&cacute;</trd>
    <trd>wzdyma&cacute; si&eogonek;</trd>
    <trd>nabrzmiewa&cacute;</trd>
    <trd>wzbiera&cacute;</trd>
    <trd>rosn&aogonek;&cacute;</trd>
    <trd>powi&eogonek;ksza&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_u;&c_h;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_z;&c_d;&c_u;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="sxvel.0ajxo">
  <kap>
  <tld/>a&jcirc;o
  </kap>
  <snc>
    <dif>
      Io <tld/>inta
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="tuber.0o">tubero</ref>,
    <ref cel="puf.0o">pufo</ref>,
    <ref cel="tumor.0o">tumoro</ref>.
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_p;&c_u;&c_h;&c_l;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_g;&c_u;&c_z;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>boursouflure</trd>,
    <trd>enflure</trd>,
    <trd>gonflement</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>duzzanat</trd>,
    <trd>daganat</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>obrz&eogonek;k</trd>,
    <trd>obrzmienie</trd>,
    <trd>opuchlizna</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>incha&ccedil;o</trd>,
    <trd>protuber&acirc;ncia</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_p;&c_u;&c_h;&c_o;&c_l;&c_mol;</trd>,
    <trd>&c_o;&c_p;&c_u;&c_h;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_sh;&c_i;&c_sh;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="sxvel.akvo0o">
  <kap>
  akvo<tld/>o
  </kap>
  <uzo tip="fak">MED</uzo>
  <uzo tip="stl">KOMUNE</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="edem.0o">Edemo</ref>.
    </dif>
  </snc>
  <trd lng="hu">vizeny&odblac;</trd>
  <trd lng="pl">puchlina<klr> wodna</klr></trd>
  <trd lng="pt">bolha</trd>
</drv>
</art>
