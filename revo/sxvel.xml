<drv mrk="sxvel.brusto0i">
  <kap>brusto<tld/>i</kap>
  <snc mrk="sxvel.brusto0i.FRAZ">
    <uzo tip="stl">FRAZ</uzo>
    <dif>
      <ref tip="super" cel="fier.0i">Fieri</ref>, la&ubreve;vorte a&ubreve; figure rektigante la torson,
      <ref tip="vid" cel="fanfar1.0i">fanfaroni</ref>,
      <ref tip="vid" cel="glor.sin0i">sin glori</ref>,
      <ref tip="vid" cel="pav.0i.FIG">pavi<sncref/></ref>:
      <ekz>
        personoj <klr>[&#x2026;]</klr> disradias fierecon 
        pri sia bonega angla lingvo, dum fakte ilia nivelo tre 
        distancas de tio, pri kio efektive eblus brusto<tld/>i
        <fnt>
          <aut>C. Piron</aut>,
          <vrk><url ref="http://claudepiron.free.fr/articlesenesperanto/kompari.htm">
          Observi, kompari, elekti</url></vrk>,
          <lok>[vidita en 2011]</lok>
        </fnt>;
      </ekz>
      <ekz>
        li brusto<tld/>is anta&ubreve; la &jcirc;urnalistoj
        <fnt>
          <bib>MD17</bib>
          <vrk>Post la usonaj frapoj en Sirio</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>sich in die <ind>Brust</ind> werfen</trd>,
      <trd>sich <ind>wichtig</ind> machen</trd>
    </trdgrp>
    <trd lng="fr">plastronner</trd>
    <trdgrp lng="it">
      <trd>gonfiarsi (essere impettito)</trd>,
      <trd>essere <ind>impettito</ind></trd>
    </trdgrp>
  </snc>
</drv>

