<drv mrk="sxvel.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sxvel.mal0i.KOMUNE">
    <dif>
      Maldiki&gcirc;i, malvasti&gcirc;i, malmulti&gcirc;i:
      <ekz>
        kiam la vento malpliforti&gcirc;as, kaj e&ccirc; ne plu blovas, la maro mal<tld/>as
        <fnt>
          <aut>M. Rollet de l'Isle red.</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/MaristaTerminaro/MaristaTerminaro.html">
          Provo de Marista Terminaro</url></vrk>,
          <lok>1908</lok>
        </fnt>;
      </ekz>
      <ekz>
        la revo de <nom>Zamenhof</nom> pri rapida disvastigo de lia komunikilo devis mal<tld/>i
        <fnt>
          <bib>Monato</bib>
          <aut>Claude Piron</aut>
          <vrk><url ref="&Monato;/2004/008264.html">Funkcio kreas organon</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_a;&c_d;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_p;&c_r;&c_a; &c_a;&c_p;&c_u;&c_h;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;)</klr></trd>
