<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxvel.xml,v 1.31 2022/05/05 14:43:49 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;vel</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sxvel.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sxvel.0i.plivolumene">
    <dif>
      Plivolumeni&gcirc;i, pli<ref cel="dik.0igxi">diki&gcirc;i</ref>,  
      <ref cel="vast.pli0igxi">plivasti&gcirc;i</ref>:
      <ekz>
        mia viza&gcirc;o <tld/>is de plorado
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 16:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sango amasi&gcirc;is en liaj piedoj, kiuj <tld/>is kaj forte doloris
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sunfloroj havis tute <tld/>intajn kapojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Lekanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vojo kondukas trans <tld/>intaj riveroj
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la brakoj la muskoloj tiel <tld/>is, ke la ha&ubreve;to preska&ubreve; krevis
        <fnt>
          <bib>IKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        iliaj trezorejoj <klr>[&#x2026;]</klr> komencis <tld/>i kaj 
        hodia&ubreve; estas tiel dikaj de oro
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        la akvo apud la &scirc;ipeto komencis malklari&gcirc;i kaj <tld/>i en grandajn ondojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i levis la pezajn, <tld/>intaj palpebrojn
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dik.0igxi">diki&gcirc;i</ref>,
      <ref cel="etend.0igxi">etendi&gcirc;i</ref>,
      <ref cel="kresk.0i">kreski</ref>,
      <ref cel="krev.0i">krevi</ref>,
      <ref cel="pli.0igxi">plii&gcirc;i</ref>,
      <ref cel="puf.0igxi">pufi&gcirc;i</ref>
    </refgrp>
    <trdgrp lng="pt">
      <trd>inchar</trd>,
      <trd>aumentar <klr>(de volume)</klr></trd>,
      <trd>avolumar-se</trd>,
      <trd>crescer</trd>,
      <trd>expandir-se</trd>
    </trdgrp>
  </snc>
  <snc mrk="sxvel.0i.sente">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tropleni&gcirc;i de iu emocio:
      <ekz>
        la princo <tld/>is de la fiero kaj lian koron plenigis pli kaj pli forta kolero
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi kvaza&ubreve; <tld/>is de abomeno kaj timo
        <fnt>
          <bib>FdO</bib>
        </fnt>;
      </ekz>
      <ekz>
        la bona knabego <tld/>is, sin sentis grava persono
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        lia koro <tld/>is de kompato
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>parola
        <klr>(bombasta)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="absorb.0igxi">absorbi&gcirc;i</ref>
    </refgrp>
    <trdgrp lng="pt">
      <trd>alargar-se</trd>,
      <trd>avolumar-se</trd>,
      <trd>dilatar-se</trd>,
      <trd>expandir-se</trd>
    </trdgrp>
  </snc>
  <snc mrk="sxvel.0i.iel">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Pligrandi&gcirc;i la&ubreve; iu ajn rilato, plialti&gcirc;i,
      plimulti&gcirc;i, pliforti&gcirc;i:
      <ekz>
        la stre&ccirc;ovundita kaj malsanula vo&ccirc;o de Pilato <tld/>is <klr>[&gcirc;is]</klr> la
        hegemono kriegis
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        la konfido en la virtoj de la ekonomia kaj financa tutmondi&gcirc;o serioze mal<tld/>is
        <fnt>
          <bib>MD02</bib>
          <vrk>La &leftquot;aliancanoj&rightquot; en la unua vico por protekti la imperion</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la prezo de <klr>[&#x2026;]</klr> importita rizo dan&gcirc;ere <tld/>as
        <fnt>
          <bib>MD08</bib>
          <vrk>Spekulacia spiralo, eksplodo de la postulo, disvolvo de agrobrula&jcirc;oj</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pt">
      <trd>avolumar-se</trd>,
      <trd>inchar</trd>,
      <trd>aumentar</trd>
    </trdgrp>
  </snc>
<!--
  <snc>
    <dif>
      Tromulti&gcirc;i, trokreski:
      <ekz>
        la novaj merkatoj estas nekapablaj engluti la &ccirc;iam pli
        <tld/>intan produktadon de l' industrio.
      </ekz>
    </dif>
    <trdgrp lng="pt">
      <trd>aumentar</trd>,
      <trd>crescer</trd>,
      <trd>expandir-se</trd>
    </trdgrp>
  </snc>
-->
  <trdgrp lng="be">
    <trd>&c_p;&c_u;&c_h;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_p;&c_u;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_b;&c_r;&c_y;&c_n;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_b;&c_r;&c_a;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schwellen</trd>,
    <trd>sich <ind>aufbl&auml;hen</ind></trd>,
    <trd>quellen</trd>
  </trdgrp>
  <trd lng="en">swell</trd>
  <trdgrp lng="fr">
    <trd>enfler</trd>,
    <trd>gonfler<klr> (intr.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>duzzad</trd>,
    <trd>dagad</trd>,
    <trd>n&ouml;vekszik<klr tip="ind"> (&aacute;tv.)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>gonfiarsi</trd>,
    <trd>ingrossarsi</trd>,
    <trd>lievitare<klr> (fig.)</klr></trd>
  </trdgrp>
  <trd lng="nl">zwellen</trd>
  <trdgrp lng="pl">
    <trd>puchn&aogonek;&cacute;</trd>
    <trd>wzdyma&cacute; si&eogonek;</trd>
    <trd>nabrzmiewa&cacute;</trd>
    <trd>wzbiera&cacute;</trd>
    <trd>rosn&aogonek;&cacute;</trd>
    <trd>powi&eogonek;ksza&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_u;&c_h;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_z;&c_d;&c_u;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
<trdgrp lng="th">
    <trd>&#xE02;&#xE22;&#xE32;&#xE22;</trd>,
    <trd>&#xE42;&#xE15;&#xE02;&#xE36;&#xE49;&#xE19;</trd>,
    <trd>&#xE43;&#xE2B;&#xE0D;&#xE48;&#xE02;&#xE36;&#xE49;&#xE19;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x443;&#x445;&#x43D;&#x443;&#x442;&#x438;</trd>,
    <trd>&#x43E;&#x43F;&#x443;&#x445;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43D;&#x430;&#x431;&#x443;&#x445;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43D;&#x430;&#x431;&#x440;&#x44F;&#x43A;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x437;&#x434;&#x443;&#x432;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>,
    <trd>(&#x43F;&#x440;&#x43E; &#x43D;&#x430;&#x441;&#x456;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x431;&#x440;&#x443;&#x43D;&#x44C;&#x43A;&#x438; &#x449;&#x435;) &#x431;&#x443;&#x431;&#x43D;&#x44F;&#x432;&#x456;&#x442;&#x438;</trd>
  </trdgrp>
</drv>


<drv mrk="sxvel.0a">
  <kap>
    <tld/>a
  </kap>
  <snc mrk="sxvel.0a.KOMUNE">
    <dif>
      <tld lit="&Scirc;"/>anta a&ubreve; <tld/>inta:
      <ekz>
        kovrita per ru&gcirc;aj <tld/>aj aknoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj, brulaj okuloj
        <fnt>
          <bib>LGA</bib>
        </fnt>;
      </ekz>
      <ekz>
        alia kun <tld/>aj lipoj kaj okulo bluverde flava
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la apuda tablo vidi&gcirc;as ies sango<tld/>aj bov&#x2019;okuloj
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        viza&gcirc;o <klr>[&#x2026;]</klr> plena de sin&scirc;an&gcirc;aj avidoj, <tld/>a de graso
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        li sentis sin <tld/>a je &gcirc;ojo infana
        <fnt>
          <bib>ChC</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia volumeno estis ege ampleksa kaj &scirc;iaj astronomie <tld/>aj konturoj ka&ubreve;zis
        angoron al junaj viroj,
        <fnt>
          <bib>KKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiuj kostumitaj, <tld/>aj kaj pezaj dramoj estis supozataj konveni al la gustoj
        de la islamanoj
        <fnt>
          <bib>MD02</bib>
          <vrk>&Ccirc;u prefere klasike a&ubreve; dialekte?</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>schw&uuml;lstig</trd>,
    <trd>geschwollen</trd>
  </trdgrp>
<trdgrp lng="fr">
  <trd>enfl&eacute;</trd>,
  <trd>gonfl&eacute;</trd>
</trdgrp></drv>


<drv mrk="sxvel.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxvel.0o.KOMUNE">
    <dif>
      Diki&gcirc;o, pligrandi&gcirc;o, ampleksi&gcirc;o:
      <ekz>
        mi havas belan <tld/>on, sed serioza &gcirc;i ne aspektas
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ipo ruli&gcirc;as, se &gcirc;i ricevas <tld/>on <klr>(kp <ref cel="hul.0o.KOMUNE">hulo</ref>)</klr> 
        de flanke <klr>[kaj]</klr> tangas, se &gcirc;i ricevas <tld/>on de anta&ubreve;e a&ubreve; deposte
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        la balono krevis pro tro-<tld/>o
        <fnt>
          <aut>La&ubreve;lum</aut>,
          <vrk><url ref="http://esperanto.china.org.cn/world/shi-window/lernado/17-1.htm">
          Vortareto por lernantoj</url></vrk>,
          <lok>[2004?]</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dilat.0o">dilato</ref>,
      <ref cel="vezik.0o">veziko</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Schwellung</trd>,
    <trd>Schwall</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>enflure</trd>,
    <trd>gonflement</trd>
  </trdgrp>
</drv>

<drv mrk="sxvel.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="sxvel.0ajxo.KOMUNE">
    <dif>
      Io <tld/>inta:
      <ekz>
        blanka <tld/>a&jcirc;o sur la ha&ubreve;to
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 13:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        la panoj estis torditaj en fantaziajn spiralojn kaj <tld/>a&jcirc;ojn
        <fnt>
          <aut>Anna L&ouml;wenstein</aut>
          <vrk>La &scirc;tona urbo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la muroj &ccirc;e multaj lokoj havis &gcirc;ibojn kaj <tld/>a&jcirc;ojn
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Sinjorino Ajuolo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akn.0o.sxvelajxo">akno</ref>,
      <ref cel="gxib.0o">&gcirc;ibo</ref>,
      <ref cel="puf.0o">pufo</ref>,
      <ref cel="pustul.0o">pustulo</ref>,
      <ref cel="tuber.0o">tubero</ref>,
      <ref cel="tumor.0o">tumoro</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_u;&c_h;&c_l;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_g;&c_u;&c_z;</trd>
  </trdgrp>
  <trd lng="bo">&t_cha;&t_shabchu;&t_tsheg;&t_sa;&t_gata;&t_nga;&t_tsheg;</trd>
  <trdgrp lng="de">
    <trd>Beule</trd>,
    <trd>Schwellung</trd>,
    <trd>Geschwulst</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>boursouflure</trd>,
    <trd>enflure</trd>,
    <trd>gonflement</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>duzzanat</trd>,
    <trd>daganat</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>gonfiore</trd>,
    <trd>rigonfiamento</trd>, 
    <trd>tumefazione</trd>
  </trdgrp>
  <trd lng="nl">gezwel</trd>
  <trdgrp lng="pl">
    <trd>obrz&eogonek;k</trd>,
    <trd>obrzmienie</trd>,
    <trd>opuchlizna</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>incha&ccedil;o</trd>,
    <trd>protuber&acirc;ncia</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_p;&c_u;&c_h;&c_o;&c_l;&c_mol;</trd>,
    <trd>&c_o;&c_p;&c_u;&c_h;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_sh;&c_i;&c_sh;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x43F;&#x443;&#x445;</trd>,
    <trd>&#x43F;&#x443;&#x445;&#x43B;&#x438;&#x43D;&#x430;</trd>,
    <trd>&#x43D;&#x430;&#x431;&#x440;&#x44F;&#x43A;</trd>
  </trdgrp>
</drv>

<drv mrk="sxvel.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxvel.0igi.KOMUNE">
    <dif>
      Ka&ubreve;zi <tld/>on, diki&gcirc;on:
      <ekz>
        amo ne fanfaronas, ne <tld/>igas sin
        <fnt>
          <bib>NT</bib>
          <lok>&Ko1; 13:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        la scio blove<tld/>igas, sed la amo edifas
        <fnt>
          <bib>NT</bib>
          <lok>&Ko1; 8:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiuj kantoj sen vortoj <klr>[&#x2026;]</klr> &ccirc;iam pli potence <tld/>igis la bruston de
        la birdo
        <fnt>
          <bib>Fab4</bib>
          <lok>Knabino, kiu pa&scirc;is sur panon</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la diablo <tld/>igis la splitojn tiel, ke ili fari&gcirc;is traboj, kaj tiam liaj
        okuloj pereis
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="strecx.0i">stre&ccirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="de">aufbl&auml;hen</trd>
  <trdgrp lng="fr">
    <trd>faire <ind>enfler</ind></trd>,
    <trd>faire <ind>gonfler</ind></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x438;&#x43A;&#x43B;&#x438;&#x43A;&#x430;&#x442;&#x438; &#x43E;&#x43F;&#x443;&#x445;&#x438;</trd>,
    <trd>&#x43D;&#x430;&#x434;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x437;&#x434;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x437;&#x434;&#x438;&#x43C;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="sxvel.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sxvel.mal0i.KOMUNE">
    <dif>
      Maldiki&gcirc;i, malvasti&gcirc;i, malmulti&gcirc;i:
      <ekz>
        kiam la vento malpliforti&gcirc;as, kaj e&ccirc; ne plu blovas, la maro mal<tld/>as
        <fnt>
          <aut>M. Rollet de l'Isle red.</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/MaristaTerminaro/MaristaTerminaro.html">
          Provo de Marista Terminaro</url></vrk>,
          <lok>1908</lok>
        </fnt>;
      </ekz>
      <ekz>
        la revo de <nom>Zamenhof</nom> pri rapida disvastigo de lia komunikilo devis mal<tld/>i
        <fnt>
          <bib>Monato</bib>
          <aut>Claude Piron</aut>
          <vrk><url ref="&Monato;/2004/008264.html">Funkcio kreas organon</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>abschwellen</trd>,
    <trd>zur&uuml;ckgehen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;senfler</trd>,
    <trd>se <ind>d&eacute;gonfler</ind></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x43F;&#x430;&#x434;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x441;&#x43F;&#x430;&#x434;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x441;&#x43B;&#x430;&#x431;&#x448;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x442;&#x440;&#x430;&#x447;&#x430;&#x442;&#x438; &#x456;&#x43D;&#x442;&#x435;&#x43D;&#x441;&#x438;&#x432;&#x43D;&#x456;&#x441;&#x442;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="sxvel.mal0o">
  <kap>mal<tld/>o</kap>
  <snc mrk="sxvel.mal0o.KOMUNE">
    <dif>
      Maldiki&gcirc;o, malampleksi&gcirc;o:
      <ekz>
        <klr>[&#x2026;]</klr> oceano, en kiu &ccirc;iu <tld/>o kaj 
        mal<tld/>o estas klare, aparte, difinite perceptata 
        <fnt>
          <aut>C. Piron</aut>,
          <vrk><url ref="http://esperanto.org.uk/eldonoj/piron/tekstoj/Tien.html">
          Tien</url></vrk>,
          <lok>1997</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">R&uuml;ckgang</trd>
