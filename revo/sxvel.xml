<drv mrk="sxvel.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sxvel.0i.plivolumene">
    <dif>
      Plivolumeni&gcirc;i, pli<ref cel="dik.0igxi">diki&gcirc;i</ref>,
      <ref cel="vast.pli0igxi">plivasti&gcirc;i</ref>:
      <ekz>
        mia viza&gcirc;o <tld/>is de plorado
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 16:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sango amasi&gcirc;is en liaj piedoj, kiuj <tld/>is kaj forte doloris
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sunfloroj havis tute <tld/>intajn kapojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Lekanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vojo kondukas trans <tld/>intaj riveroj
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la brakoj la muskoloj tiel <tld/>is, ke la ha&ubreve;to preska&ubreve; krevis
        <fnt>
          <bib>IKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        iliaj trezorejoj <klr>[&#8230;]</klr> komencis <tld/>i kaj
        hodia&ubreve; estas tiel dikaj de oro
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        la akvo apud la &scirc;ipeto komencis malklari&gcirc;i kaj <tld/>i en grandajn ondojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i levis la pezajn, <tld/>intaj palpebrojn
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dik.0igxi">diki&gcirc;i</ref>,
      <ref cel="etend.0igxi">etendi&gcirc;i</ref>,
      <ref cel="kresk.0i">kreski</ref>,
      <ref cel="krev.0i">krevi</ref>,
      <ref cel="pli.0igxi">plii&gcirc;i</ref>,
      <ref cel="puf.0igxi">pufi&gcirc;i</ref>
    </refgrp>
    <trdgrp lng="pt">
      <trd>inchar</trd>,
      <trd>aumentar <klr>(de volume)</klr></trd>,
      <trd>avolumar-se</trd>,
      <trd>crescer</trd>,
      <trd>expandir-se</trd>
    </trdgrp>
  </snc>
  <snc mrk="sxvel.0i.sente">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tropleni&gcirc;i de iu emocio:
      <ekz>
        la princo <tld/>is de la fiero kaj lian koron plenigis pli kaj pli forta kolero
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi kvaza&ubreve; <tld/>is de abomeno kaj timo
        <fnt>
          <bib>FdO</bib>
        </fnt>;
      </ekz>
      <ekz>
        la bona knabego <tld/>is, sin sentis grava persono
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        lia koro <tld/>is de kompato
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>parola
        <klr>(bombasta)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="absorb.0igxi">absorbi&gcirc;i</ref>
    </refgrp>
    <trdgrp lng="pt">
      <trd>alargar-se</trd>,
      <trd>avolumar-se</trd>,
      <trd>dilatar-se</trd>,
      <trd>expandir-se</trd>
    </trdgrp>
  </snc>
  <snc mrk="sxvel.0i.iel">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Pligrandi&gcirc;i la&ubreve; iu ajn rilato, plialti&gcirc;i,
      plimulti&gcirc;i, pliforti&gcirc;i:
      <ekz>
        la stre&ccirc;ovundita kaj malsanula vo&ccirc;o de Pilato <tld/>is <klr>[&gcirc;is]</klr> la
        hegemono kriegis
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        la konfido en la virtoj de la ekonomia kaj financa tutmondi&gcirc;o serioze mal<tld/>is
        <fnt>
          <bib>MD02</bib>
          <vrk>La &leftquot;aliancanoj&rightquot; en la unua vico por protekti la imperion</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la prezo de <klr>[&#8230;]</klr> importita rizo dan&gcirc;ere <tld/>as
        <fnt>
          <bib>MD08</bib>
          <vrk>Spekulacia spiralo, eksplodo de la postulo, disvolvo de agrobrula&jcirc;oj</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pt">
      <trd>avolumar-se</trd>,
      <trd>inchar</trd>,
      <trd>aumentar</trd>
    </trdgrp>
  </snc>
<!--
  <snc>
    <dif>
      Tromulti&gcirc;i, trokreski:
      <ekz>
        la novaj merkatoj estas nekapablaj engluti la &ccirc;iam pli
        <tld/>intan produktadon de l' industrio.
      </ekz>
    </dif>
    <trdgrp lng="pt">
      <trd>aumentar</trd>,
      <trd>crescer</trd>,
      <trd>expandir-se</trd>
    </trdgrp>
  </snc>
-->
