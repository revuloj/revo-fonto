<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxvel.xml,v 1.24 2021/05/29 10:57:50 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;vel</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sxvel.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Plivolumeni&gcirc;i, plidiki&gcirc;i,  plivasti&gcirc;i:
      <ekz>
        lia vango <tld/>as pro absceso;
      </ekz>
      <ekz>
        <tld/>inta kiel balono, kiel bufo;
      </ekz>
      <ekz>
        <tld/>inta rivero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>mola (mola kaj elasta) tapi&scirc;o.
      </ekz>
    </dif>
    <trdgrp lng="pt">
      <trd>inchar</trd>,
      <trd>aumentar <klr>(de volume)</klr></trd>,
      <trd>avolumar-se</trd>,
      <trd>crescer</trd>,
      <trd>expandir-se</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Trodiki&gcirc;i, trovasti&gcirc;i:
      <ekz>
        li <tld/>is de fiero<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>parola
        <klr>(bombasta)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="puf.0igxi">pufi&gcirc;i</ref>
    </refgrp>
    <trdgrp lng="pt">
      <trd>alargar-se</trd>,
      <trd>avolumar-se</trd>,
      <trd>dilatar-se</trd>,
      <trd>expandir-se</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Pligrandi&gcirc;i la&ubreve; iu ajn rilato, plialti&gcirc;i,
      plimulti&gcirc;i, pliforti&gcirc;i:
      <ekz>
        la kantoj grade <tld/>adis;
      </ekz>
      <ekz>
        la fusilbruado <tld/>as.
      </ekz>
    </dif>
    <trdgrp lng="pt">
      <trd>avolumar-se</trd>,
      <trd>inchar</trd>,
      <trd>aumentar</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Tromulti&gcirc;i, trokreski:
      <ekz>
        la novaj merkatoj estas nekapablaj engluti la &ccirc;iam pli
        <tld/>intan produktadon de l' industrio.
      </ekz>
    </dif>
    <trdgrp lng="pt">
      <trd>aumentar</trd>,
      <trd>crescer</trd>,
      <trd>expandir-se</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_u;&c_h;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_p;&c_u;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_b;&c_r;&c_y;&c_n;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_b;&c_r;&c_a;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">schwellen</trd>
  <trd lng="en">swell</trd>
  <trdgrp lng="fr">
    <trd>enfler</trd>,
    <trd>gonfler<klr> (intr.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>duzzad</trd>,
    <trd>dagad</trd>,
    <trd>n&ouml;vekszik<klr tip="ind"> (&aacute;tv.)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>gonfiarsi</trd>,
    <trd>ingrossarsi</trd>,
    <trd>lievitare<klr> (fig.)</klr></trd>
  </trdgrp>
  <trd lng="nl">zwellen</trd>
  <trdgrp lng="pl">
    <trd>puchn&aogonek;&cacute;</trd>
    <trd>wzdyma&cacute; si&eogonek;</trd>
    <trd>nabrzmiewa&cacute;</trd>
    <trd>wzbiera&cacute;</trd>
    <trd>rosn&aogonek;&cacute;</trd>
    <trd>powi&eogonek;ksza&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_u;&c_h;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_z;&c_d;&c_u;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
<trdgrp lng="th">
    <trd>&#xE02;&#xE22;&#xE32;&#xE22;</trd>,
    <trd>&#xE42;&#xE15;&#xE02;&#xE36;&#xE49;&#xE19;</trd>,
    <trd>&#xE43;&#xE2B;&#xE0D;&#xE48;&#xE02;&#xE36;&#xE49;&#xE19;</trd>
  </trdgrp>
</drv>

<drv mrk="sxvel.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxvel.0o.KOMUNE">
    <dif>
      Diki&gcirc;o, pligrandi&gcirc;o, ampleksi&gcirc;o:
      <ekz>
        la balono krevis pro tro-<tld/>o
        <fnt>
          <aut>La&ubreve;lum</aut>,
          <vrk><url ref="http://esperanto.china.org.cn/world/shi-window/lernado/17-1.htm">
          Vortareto por lernantoj</url></vrk>,
          <lok>[2004?]</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vezik.0o">veziko</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>enflure</trd>,
    <trd>gonflement</trd>
  </trdgrp>
</drv>

<drv mrk="sxvel.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="sxvel.0ajxo.KOMUNE">
    <dif>
      Io <tld/>inta.
    </dif>
    <refgrp tip="vid">
      <ref cel="puf.0o">pufo</ref>,
      <ref cel="tuber.0o">tubero</ref>,
      <ref cel="tumor.0o">tumoro</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_u;&c_h;&c_l;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_g;&c_u;&c_z;</trd>
  </trdgrp>
  <trd lng="bo">&#xF46;&#xF74;&#xF0B;&#xF66;&#xF92;&#xF44;&#xF0B;</trd>
  <trdgrp lng="de">
    <trd>Beule</trd>,
    <trd>Schwellung</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>boursouflure</trd>,
    <trd>enflure</trd>,
    <trd>gonflement</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>duzzanat</trd>,
    <trd>daganat</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>gonfiore</trd>,
    <trd>rigonfiamento</trd>, 
    <trd>tumefazione</trd>
  </trdgrp>
  <trd lng="nl">gezwel</trd>
  <trdgrp lng="pl">
    <trd>obrz&eogonek;k</trd>,
    <trd>obrzmienie</trd>,
    <trd>opuchlizna</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>incha&ccedil;o</trd>,
    <trd>protuber&acirc;ncia</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_p;&c_u;&c_h;&c_o;&c_l;&c_mol;</trd>,
    <trd>&c_o;&c_p;&c_u;&c_h;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_sh;&c_i;&c_sh;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="sxvel.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxvel.0igi.KOMUNE">
    <dif>
      Ka&ubreve;zi <tld/>on, diki&gcirc;on:
      <ekz>
        la vento <tld/>igas la velon;
      </ekz>
      <ekz>
        ni ne tro <tld/>igu nian vortaron;
      </ekz>
      <ekz>
        <tld/>igitaj prezoj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="strecx.0i">stre&ccirc;i</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>faire <ind>enfler</ind></trd>,
    <trd>faire <ind>gonfler</ind></trd>
  </trdgrp>
</drv>

<drv mrk="sxvel.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sxvel.mal0i.KOMUNE">
    <dif>
      Maldiki&gcirc;i, malvasti&gcirc;i, malmulti&gcirc;i:
      <ekz>
        la maro mal<tld/>as
        <fnt>
          <aut>M. Rollet de l'Isle red.</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/MaristaTerminaro/MaristaTerminaro.html">
          Provo de Marista Terminaro</url></vrk>,
          <lok>1908</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>d&eacute;senfler</trd>,
    <trd>se d&eacute;gonfler</trd>
  </trdgrp>
</drv>

<drv mrk="sxvel.mal0o">
  <kap>mal<tld/>o</kap>
  <snc mrk="sxvel.mal0o.KOMUNE">
    <dif>
      Maldiki&gcirc;o, malampleksi&gcirc;o:
      <ekz>
        <klr>...</klr> oceano, en kiu &ccirc;iu <tld/>o kaj 
        mal<tld/>o estas klare, aparte, difinite perceptata 
        <fnt>
          <aut>C. Piron</aut>,
          <vrk><url ref="http://esperanto.org.uk/eldonoj/piron/tekstoj/Tien.html">
          Tien</url></vrk>,
          <lok>1997</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">d&eacute;gonflement</trd>
</drv>

<drv mrk="sxvel.akvo0o">
  <kap>akvo<tld/>o</kap>
  <snc mrk="sxvel.akvo0o.MED">
    <uzo tip="fak">MED</uzo>
    <uzo tip="stl">KOMUNE</uzo>
    <ref tip="dif" cel="edem.0o">edemo</ref>
  </snc>
  <trd lng="de">&Ouml;dem</trd>
  <trd lng="fr">&oelig;d&egrave;me</trd>
  <trd lng="hu">vizeny&odblac;</trd>
  <trdgrp lng="it">
    <trd>edema</trd>,
    <trd>bolla<klr> (edema - com.)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>oedeem</trd>,
    <trd>waterzucht</trd>,
    <trd>hydropsie</trd>
  </trdgrp>
  <trd lng="pl">puchlina<klr> wodna</klr></trd>
  <trd lng="pt">bolha</trd>
</drv>

<drv mrk="sxvel.brusto0i">
  <kap>brusto<tld/>i</kap>
  <snc mrk="sxvel.brusto0i.FRAZ">
    <uzo tip="stl">FRAZ</uzo>
    <dif>
      Fieri, la&ubreve;vorte a&ubreve; figure rektigante la torson,
      <ref tip="vid" cel="fanfar1.0i">fanfaroni</ref>,
      <ref tip="vid" cel="glor.sin0i">sin glori</ref>,
      <ref tip="vid" cel="pav.0i.FIG">pavi<sncref/></ref>:
      <ekz>
        personoj <klr>...</klr> disradias fierecon 
        pri sia bonega angla lingvo, dum fakte ilia nivelo tre 
        distancas de tio, pri kio efektive eblus brusto<tld/>i
        <fnt>
          <aut>C. Piron</aut>,
          <vrk><url ref="http://claudepiron.free.fr/articlesenesperanto/kompari.htm">
          Observi, kompari, elekti</url></vrk>,
          <lok>[vidita en 2011]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">plastronner</trd>
    <trdgrp lng="it">
      <trd>gonfiarsi (essere impettito)</trd>,
      <trd>essere <ind>impettito</ind></trd>
    </trdgrp>
  </snc>
</drv>
</art>
