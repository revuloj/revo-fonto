<drv mrk="sxvel.0a">
  <kap>
    <tld/>a
  </kap>
  <snc mrk="sxvel.0a.KOMUNE">
    <dif>
      <tld lit="&Scirc;"/>anta a&ubreve; <tld/>inta:
      <ekz>
        kovrita per ru&gcirc;aj <tld/>aj aknoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj, brulaj okuloj
        <fnt>
          <bib>LGA</bib>
        </fnt>;
      </ekz>
      <ekz>
        alia kun <tld/>aj lipoj kaj okulo bluverde flava
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la apuda tablo vidi&gcirc;as ies sango<tld/>aj bov&#8217;okuloj
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        viza&gcirc;o <klr>[&#8230;]</klr> plena de sin&scirc;an&gcirc;aj avidoj, <tld/>a de graso
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        li sentis sin <tld/>a je &gcirc;ojo infana
        <fnt>
          <bib>ChC</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia volumeno estis ege ampleksa kaj &scirc;iaj astronomie <tld/>aj konturoj ka&ubreve;zis
        angoron al junaj viroj,
        <fnt>
          <bib>KKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiuj kostumitaj, <tld/>aj kaj pezaj dramoj estis supozataj konveni al la gustoj
        de la islamanoj
        <fnt>
          <bib>MD02</bib>
          <vrk>&Ccirc;u prefere klasike a&ubreve; dialekte?</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_p;&c_u;&c_h;&c_l;&c_y;</trd>,
    <trd>&c_a;&c_z;&c_y;&c_z;&c_l;&c_y;</trd>,
    <trd>&c_n;&c_a;&c_b;&c_r;&c_a;&c_k;&c_l;&c_y;</trd>,
    <trd>&c_n;&c_a;&c_b;&c_u;&c_h;&c_l;&c_y;</trd>,
    <trd>&c_n;&c_a;&c_b;&c_r;&c_y;&c_n;&c_ja;&c_l;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schw&uuml;lstig</trd>,
    <trd>geschwollen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>enfl&eacute;</trd>,
    <trd>gonfl&eacute;</trd>
   </trdgrp>
   <trd lng="pl">nabrzmia&lstroke;y</trd>
  <trdgrp lng="zh">
    <trd>&#33192; <pr>p&eacute;ng</pr></trd>,
    <trd>&#33152; <pr>p&amacron;ng</pr></trd>,
    <trd>&#32959; <pr>zh&#466;ng</pr></trd>,
    <trd>&#33131; <pr>zh&#466;ng</pr></trd>,
    <trd>&#27873; <pr>p&amacron;o</pr></trd>,
    <trd>&#33053; <pr>h&emacron;ng</pr></trd>,
    <trd>&#33238; <pr>x&igrave;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxvel.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxvel.0o.KOMUNE">
    <dif>
      Diki&gcirc;o, pligrandi&gcirc;o, ampleksi&gcirc;o:
      <ekz>
        mi havas belan <tld/>on, sed serioza &gcirc;i ne aspektas
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ipo ruli&gcirc;as, se &gcirc;i ricevas <tld/>on <klr>(kp <ref cel="hul.0o.KOMUNE">hulo</ref>)</klr>
        de flanke <klr>[kaj]</klr> tangas, se &gcirc;i ricevas <tld/>on de anta&ubreve;e a&ubreve; deposte
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        la balono krevis pro tro-<tld/>o
        <fnt>
          <aut>La&ubreve;lum</aut>,
          <vrk><url ref="http://esperanto.china.org.cn/world/shi-window/lernado/17-1.htm">
          Vortareto por lernantoj</url></vrk>,
          <lok>[2004?]</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dilat.0o">dilato</ref>,
      <ref cel="vezik.0o">veziko</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_p;&c_u;&c_h;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_u;&c_h;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schwellung</trd>,
    <trd>Schwall</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>enflure</trd>,
    <trd>gonflement</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#33192;&#24373;  <pr>&#12412;&#12358;&#12385;&#12423;&#12358;</pr></trd>,
    <trd>&#33131;&#12428;  <pr>&#12399;&#12428;</pr></trd>
  </trdgrp>
  <trd lng="pl">obrz&eogonek;k</trd>
  <trdgrp lng="zh">
    <trd>&#32959;&#32960; <pr>zh&#466;ngzh&agrave;ng</pr></trd>,
    <trd>&#33131;&#33081; <pr>zh&#466;ngzh&agrave;ng</pr></trd>,
    <trd>&#33238; <pr>x&igrave;ng</pr></trd>
  </trdgrp>
</drv>

