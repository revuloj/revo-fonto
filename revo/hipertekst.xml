<art mrk="$Id: hipertekst.xml,v 1.5 2023/11/15 17:08:00 revo Exp $">
<kap>
    <rad>hipertekst</rad>/o <fnt><bib>KompLeks</bib></fnt>
</kap>

    <drv mrk="hipertekst.0o">
      <kap><tld/>o</kap>
      <snc mrk="hipertekst.0o.KOMP">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Teksto preparita por legado per komputilo, eventuale kun
          bildoj kaj sonoj, provizita je <tld/>aj ligiloj
          (specialaj referencoj, facile sekveblaj per musklako a&ubreve;
          klavofrapo):
          <ekz>
            DICE disponas anka&ubreve; pri foliumilo
            direktebla per klavoj, per kiu oni povas legi
            <tld/>ojn.
            <fnt>
              <aut>E. Georgiev</aut>,
              <vrk><url ref="&Monato;/1999/006436.html">Retpo&scirc;ta&jcirc;oj
                  venos ekde nun anka&ubreve; telefone</url></vrk>,
              <bib>Monato</bib>,
              <lok>1999/06</lok>.
            </fnt>
          </ekz>
        </dif>
        <ref tip="sub" cel="pagx.ret0aro">retpa&gcirc;aro</ref>
      </snc>
      <trd lng="be">&c_g;&c_ib;&c_p;&c_e;&c_r;&c_t;&c_e;&c_k;&c_s;&c_t;
      </trd>
      <trd lng="de">Hypertext</trd>
      <trd lng="en">hypertext</trd>
      <trd lng="es">hipertexto</trd>
      <trd lng="fr">hypertexte</trd>
      <trdgrp lng="hu">
        <trd>hipertext</trd>,
        <trd>hipersz&ouml;veg</trd>
      </trdgrp>
      <trd lng="ja">&#x30CF;&#x30A4;&#x30D1;&#x30FC;&#x30C6;&#x30AD;&#x30B9;&#x30C8;</trd>
  <trd lng="nl">hypertekst</trd>
      <trd lng="pl">hipertekst</trd>
      <trd lng="ru">&c_g;&c_i;&c_p;&c_je;&c_r;&c_t;&c_je;&c_k;&c_s;&c_t;</trd>
      <trd lng="sk">hypertext</trd>
      <trd lng="sv">hypertext</trd>
