<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: debat.xml,v 1.25 2021/05/28 12:14:53 revo Exp $">
    <kap>
      <ofc>2</ofc>
      <rad>debat</rad>/o
    </kap>

    <drv mrk="debat.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Diskuto la&ubreve; ordigita maniero en oficiala kunveno (kongreso,
          parlamento ktp):
          <ekz>
            la le&gcirc;o estis akceptata sen longaj <tld/>oj.
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_d;&c_e;&c_b;&c_a;&c_t;&c_y;</trd>
  <trd lng="bo">&#xF62;&#xFA9;&#xF7C;&#xF51;&#xF0B;&#xF54;&#xF0B;</trd>
  <trd lng="ca">debat</trd>
  <trdgrp lng="cs">
    <trd>debata</trd>,
    <trd>rozprava</trd>
  </trdgrp>
  <trdgrp lng="de">
        <trd>Debatte</trd>,
        <trd>Er&ouml;rterung</trd>,
        <trd>Aussprache</trd>
      </trdgrp>
  <trd lng="en">debate</trd>
  <trd lng="es">debate</trd>
  <trd lng="fa">&a_b;&a_H;&a_th;</trd>
  <trd lng="fr">d&eacute;bat</trd>
  <trdgrp lng="he">
    <trd>&#x5D3;&#x5D9;&#x5D5;&#x5DF;</trd>,
    <trd>&#x5D5;&#x5D9;&#x5DB;&#x5D5;&#x5D7;</trd>
  </trdgrp>
  <trdgrp lng="hu">
        <trd>vita</trd>,
        <trd>megvitat&aacute;s</trd>,
        <trd>megt&aacute;rgyal&aacute;s</trd>
      </trdgrp>
  <trd lng="nl">debat</trd>
  <trdgrp lng="pl">
        <trd>debata</trd>,
        <trd>obrady<klr> {pl.}</klr></trd>
      </trdgrp>
  <trdgrp lng="pt">
        <trd>debate</trd>,
        <trd>discuss&atilde;o</trd>
      </trdgrp>
  <trdgrp lng="ru">
        <trd>&c_d;&c_je;&c_b;&c_a;&c_t;&c_y;</trd>,
        <trd>&c_p;&c_r;&c_je;&c_n;&c_i;&c_ja;</trd>
      </trdgrp>
  <trdgrp lng="sk">
    <trd>debata</trd>,
    <trd>rozprava</trd>
  </trdgrp>
  <trd lng="sv">debatt</trd>
</drv>

    <drv mrk="debat.0i">
      <kap><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Oficiale <ref tip="super" cel="diskut.0i">diskuti</ref>.  
        </dif>
      </snc>
      <ref tip="hom" cel="bat.de0i">debati</ref>
      <trd lng="be">&c_d;&c_e;&c_b;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
      <trd lng="ca">debatre</trd>
      <trdgrp lng="de">
        <trd>debattieren</trd>,
        <trd>er&ouml;rtern</trd>
      </trdgrp>
      <trd lng="en">debate</trd>
      <trd lng="es">debatir</trd>
      <trd lng="fa">&a_b;&a_H;&a_th; &f_k;&a_r;&a_d;&a_n;</trd>
      <trd lng="fr">d&eacute;battre</trd>
      <trd lng="he">&#x5DC;&#x5D4;&#x5EA;&#x5D5;&#x5D5;&#x5DB;&#x5D7;</trd>
  <trdgrp lng="hu">
        <trd>megvitat</trd>,
        <trd>megt&aacute;rgyal</trd>
      </trdgrp>
      <trd lng="nl">debatteren</trd>
      <trdgrp lng="pl">
        <trd>debatowa&cacute;</trd>,
        <trd>prowadzi&cacute; <ind>obrady</ind></trd>,
        <trd>radzi&cacute;<klr> (nad czym&sacute;)</klr></trd>
      </trdgrp>
      <trdgrp lng="pt">
        <trd>debater</trd>,
        <trd>discutir</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_d;&c_je;&c_b;&c_a;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_o;&c_b;&c_s;&c_u;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_v;&c_je;&c_s;&c_t;&c_i; <ind>&c_p;&c_r;&c_je;&c_n;&c_i;&c_ja;</ind></trd>
      </trdgrp>
      <trd lng="sk">debatova&tcaron;</trd>
      <trd lng="sv">debattera</trd>
      <trdgrp lng="cs">
    <trd>debatovat</trd>,
    <trd>diskutovat</trd>
  </trdgrp>
</drv>
  </art>
