<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hup.xml,v 1.21 2025/03/30 16:21:22 revo Exp $">
<kap>
  <rad>hup</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="hup.0o">
  <kap><tld/>o</kap>
  <snc mrk="hup.0o.SON">
    <uzo tip="fak">AUT</uzo>
    <dif>
      Sonorigilo de veturilo:
      <ekz>
        &scirc;i ne permesis, ke niaj amikoj sonorigu la <tld/>on por
        averti
        pri
        la horo de nia foriro
        <fnt>
        <aut>C. Hecktheuer, trad. F. J. G. Marinho</aut>,
        <vrk><url ref="http://www.npoint.com.br/sementeira/kruelajpatrinoj.html">Kruelaj
        patrinoj</url></vrk>,
        <lok>2005-07-28</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_y;&c_g;&c_n;&c_a;&c_l; <klr>(&c_a;&c_w;&c_t;&c_a;&c_m;&c_a;&c_b;&c_ib;&c_l;&c_mol;&c_n;&c_y;)</klr></trd>,
    <trd>&c_k;&c_l;&c_a;&c_k;&c_s;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="bg">&c_k;&c_l;&c_a;&c_k;&c_s;&c_o;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>chocholka</trd>,
    <trd>chom&aacute;&ccaron;</trd>
  </trdgrp>
  <trd lng="de">Hupe</trd>
  <trdgrp lng="en">
    <trd>claxon (brite)</trd>,
    <trd>car horn (usone)</trd>
  </trdgrp>
  <trd lng="es">bocina</trd>
  <trd lng="fa">&a_b;&a_w;&a_q;</trd>
  <trdgrp lng="fr">
    <trd>avertisseur</trd>,
    <trd>klaxon</trd>,
    <trd>sir&egrave;ne<klr> (de v&eacute;hicule)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
  <trd>aut&oacute;duda</trd>,
  <trd>aut&oacute;k&uuml;rt</trd>
</trdgrp>
  <trdgrp lng="it">
    <trd>tromba<klr> (di automobile)</klr></trd>,
    <trd>clacson</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#20896;&#27611;  <pr>&#12363;&#12435;&#12418;&#12358;</pr></trd>,
    <trd>&#12405;&#12373;&#27611;  <pr>&#12405;&#12373;&#12369;</pr></trd>,
    <trd>&#35686;&#31515;  <pr>&#12369;&#12356;&#12390;&#12365;</pr></trd>,
    <trd>&#12463;&#12521;&#12463;&#12471;&#12519;&#12531;</trd>
  </trdgrp>
  <trd lng="nl">Claxon</trd>
  <trdgrp lng="pl">
    <trd>klakson</trd>,
    <trd>sygna&lstroke; d&zacute;wi&eogonek;kowy</trd>
  </trdgrp>
  <trd lng="pt">buzina <klr> de autom&oacute;vel</klr></trd>
  <trdgrp lng="sk">
    <trd>po&lcaron;nica</trd>,
    <trd>signaliza&ccaron;n&aacute; tr&uacute;bka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&c_gu;&#1085;&c_a;&#1083;&#1100;&#1085;&c_i;&c_j; &c_r;&c_ib;&c_zh;&#1086;&#1082;</trd>,
    <trd>&#1082;&#1083;&c_a;&#1082;&c_s;&#1086;&#1085;</trd>
  </trdgrp>
  <trd lng="zh">&#22047; <pr>d&#363;</pr></trd>
</drv>

<drv mrk="hup.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="hup.0i.SON">
    <dif>
      Sonigi la <tld/>on de veturilo, averte, signale, salute...:
      <ekz>
        du a&ubreve;tomobiloj parkitaj &ccirc;e la enirejo <tld/>is
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru (avert-)<ke>soni/igi</ke>.
  </rim>

  <trd lng="be">&c_s;&c_y;&c_g;&c_n;&c_a;&c_l;&c_ib;&c_c;&c_mol; <klr>(&c_a;&c_b; &c_a;&c_w;&c_t;&c_a;&c_m;&c_a;&c_b;&c_ib;&c_l;&c_ib;)</klr></trd>
  <trdgrp lng="bg">
    <trd>&c_d;&c_a;&c_v;&c_a;&c_m; &c_s;&c_i;&c_g;&c_n;&c_a;&c_l; &c_s;
    &c_k;&c_l;&c_a;&c_k;&c_s;&c_o;&c_n;</trd>,
    <trd>&c_b;&c_i;&c_b;&c_i;&c_p;&c_k;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="cs">zahoukat</trd>
  <trd lng="de">hupen</trd>
  <trdgrp lng="en">
    <trd>hoot (brite)</trd>,
    <trd>honk (a car horn) (usone)</trd>
  </trdgrp>
  <trd lng="es">tocar la bocina</trd>
  <trd lng="fa">&a_b;&a_w;&a_q; &a_z;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>corner<klr> (klaxonner)</klr></trd>,
    <trd>klaxonner</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>dud&aacute;l</trd>,
    <trd>k&uuml;rt&ouml;l</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>strombazzare</trd>,
    <trd>suonare il <ind>clacson</ind></trd>
  </trdgrp>
  <trd lng="ja">&#35686;&#31515;&#12434;&#40180;&#12425;&#12377;  <pr>&#12369;&#12356;&#12390;&#12365;&#12434;&#12394;&#12425;&#12377;</pr></trd>
  <trd lng="nl">toeteren</trd>
  <trdgrp lng="pl">
    <trd>tr&aogonek;bi&cacute;<klr> (klaksonem)</klr></trd>,
    <trd>dawa&cacute; sygna&lstroke; d&zacute;wi&eogonek;kowy</trd>
  </trdgrp>
  <trd lng="sk">zah&uacute;ka&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &c_r;&c_ib;&c_zh;&#1082;&#1086;&#1084; &c_s;&c_i;&c_gu;&#1085;&c_a;&#1083;&c_i;</trd>,
    <trd>&c_s;&c_i;&c_gu;&#1085;&c_a;&#1083;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#25353;&#21895;&#21485; <pr>&#224;nl&#462;ba</pr></trd>,
    <trd>&#40180;&#34399; <pr>m&#237;ngh&#224;o</pr></trd>
  </trdgrp>
</drv>
</art>
