<drv mrk="danc.0o">
  <kap><tld/>o</kap>
  <snc mrk="danc.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Ritma movado de la membroj; aparta speco de tia okupo:
      <ekz>
        ili gloru Lian nomon en <tld/>o, per tamburino kaj harpo ili
        muziku al Li
        <fnt><bib>MT</bib>, <lok>&Psa; 149:3</lok></fnt>;
      </ekz>
      <ekz>
        balda&ubreve; espereble <klr>[iu]</klr> invitos &scirc;in al <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni vidis iliajn kamparajn <tld/>ojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        komenci&gcirc;is la arto<tld/>o, a&ubreve;, kiel ili tion nomis, la <tld/>eli&gcirc;ado
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la patrino <klr>[&#x2026;]</klr> rigardis religiajn <tld/>ojn
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &hcirc;oro de pastrinoj plenumis solenan <tld/>on
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        duonnudaj knabinoj komencis kolektivan <tld/>on
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>osaltas kiel falko, sed rilate al la plat<tld/>o mi tute ne sukcesas
        <fnt>
          <bib>DKM</bib>
          <lok>&Ccirc;apitro LXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        greka milit<tld/>o
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LXXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        folkloraj <tld/>oj
        <fnt>
          <bib>EeP</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> fleksis la gambojn kvaza&ubreve; preparante sin al ka&ubreve;r<tld/>o
        <fnt>
          <bib>MkM</bib>
          <lok>20</lok>
        </fnt>;
      </ekz>
      <ekz>
        la diafanaj niksinoj interrompis sian surriveran <ind>rondo<tld/>o</ind>n
        <fnt>
          <bib>MkM</bib>
          <lok>21</lok>
        </fnt>;
        <trd lng="de">Reigen</trd>
      </ekz>
      <ekz>
        la furiozo de la vivo estas nur vegrinco, <ind>morto<tld/>o</ind>, al kiu la malri&ccirc;o tordas
        la gurdon
        <fnt>
          <bib>SatirRak</bib>
          <lok>Liriko</lok>
        </fnt>;
        <trd lng="de">Totentanz</trd>
      </ekz>
      <ekz>
        <nom>Gustav</nom> saltis sur se&gcirc;on kaj aran&gcirc;is tambure intensan <ind>pa&scirc;<tld/>o</ind>n
        <fnt>
          <bib>VRA</bib>
          <lok>Lars Ahlin</lok>
        </fnt>;
        <trd lng="de">Schreittanz</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la muta fantoma <tld/>o de la ma&scirc;inoj
        <fnt>
          <bib>VaK</bib>
          <lok>Dua &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
    <lstref lst="voko:dancoj"><tld/>oj</lstref>
    <refgrp tip="vid">
      <ref cel="balet.0o">baleto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_n;&c_je;&c_c;</trd>,
    <trd>&c_s;&c_k;&c_o;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="bg">&c_t;&c_a;&c_n;&c_c;</trd>
  <trd lng="bo">&t_zha;&t_ba;&t_sa;&t_tsheg;&t_ba;&t_rata;&t_naro;&t_tsheg;</trd>
  <trdgrp lng="br">
    <trd>da&ntilde;s</trd>,
    <trd>koroll</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>ball</trd>,
    <trd>dansa</trd>
  </trdgrp>
  <trd lng="cs">tanec</trd>
  <trd lng="de">Tanz</trd>
  <trd lng="en">dance</trd>
  <trdgrp lng="es">
    <trd>baile</trd>,
    <trd>danza</trd>
  </trdgrp>
  <trd lng="fr">danse<klr tip="ind"> (action, mani&egrave;re de
  danser)</klr></trd>
  <trd lng="he">&resh;&yod;&qof;&vav;&dalet;</trd>
  <trd lng="hu">t&aacute;nc</trd>
  <trdgrp lng="id">
    <trd>tari</trd>,
    <trd><ind>tari</ind>an</trd>
  </trdgrp>
  <trdgrp lng="ja">
