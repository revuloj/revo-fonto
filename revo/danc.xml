<drv mrk="danc.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="danc.0isto.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Iu, kiu profesie <tld/>as:
      <ekz>
        kiel majesta estis hodia&ubreve; la re&gcirc;o de Izrael, kiu
        elmontris sin <klr>...</klr>, kiel sin elmontras iu el la
        publikaj <tld/>istoj!
        <fnt><bib>MT</bib>, <lok>&Sa2; 6:20</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_n;&c_c;&c_o;&c_r;</trd>
  <trd lng="bg">&c_t;&c_a;&c_n;&c_c;&c_mol;&c_o;&c_r;</trd>
  <trdgrp lng="br">
    <trd>da&ntilde;ser</trd>,
    <trd>koroller</trd>
  </trdgrp>
  <trd lng="ca">ballar&iacute;</trd>
  <trd lng="cs">tane&ccaron;n&iacute;k</trd>
  <trd lng="de">T&auml;nzer</trd>
  <trd lng="en">dancer</trd>
  <trd lng="es">bailar&iacute;n</trd>
  <trdgrp lng="fr">
    <trd>danseur</trd>,
    <trd>danseuse</trd>
  </trdgrp>
  <trd lng="he">&#x5E8;&#x5B7;&#x5E7;&#x5D3;&#x5B8;&#x5DF;</trd>
  <trd lng="hu">t&aacute;ncos</trd>
  <trd lng="id"><baz>tari</baz>penari<klr tip="amb">
  (laki-laki)</klr></trd>
  <trd lng="nl">danser</trd>
  <trd lng="pl">tancerz</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_a;&c_n;&c_c;&c_o;&c_v;&c_shch;&c_i;&c_k;</trd>,
    <trd>&c_p;&c_l;&c_ja;&c_s;&c_u;&c_n;</trd>
  </trdgrp>
  <trd lng="sk">tane&ccaron;n&iacute;k</trd>
</drv>

<drv mrk="danc.0istino">
  <kap><tld/>istino</kap>
  <snc mrk="danc.0istino.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Virino, kiu profesie <tld/>as:
      <ekz>
        en la subtegmenta &ccirc;ambreto staras duone
        vestita malgranda <ind><tld/>istino</ind>
        <fnt><bib>Fab2</bib>, <lok>Ne&gcirc;a re&gcirc;ino</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_a;&c_n;&c_c;&c_o;&c_r;&c_k;&c_a;</trd>
    <trd lng="ca">ballarina</trd>
    <trd lng="cs">tane&ccaron;nice</trd>
    <trd lng="de">T&auml;nzerin</trd>
    <trd lng="es">bailarina</trd>
    <trd lng="fr">danceuse</trd>
    <trd lng="hu">t&aacute;ncosn&odblac;</trd>
    <trd lng="id"><baz>tari</baz>penari<klr tip="amb">
    (perempuan)</klr></trd>
    <trd lng="nl">danseres</trd>
    <trd lng="pl">tancerka</trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_a;&c_n;&c_c;&c_o;&c_v;&c_shch;&c_i;&c_c;&c_a;</trd>,
      <trd>&c_p;&c_l;&c_ja;&c_s;&c_u;&c_n;&c_mol;&c_ja;</trd>,
      <trd>&c_b;&c_a;&c_l;&c_je;&c_r;&c_i;&c_n;&c_a;</trd>
