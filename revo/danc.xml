    <trd>&c_t;&c_a;&c_n;&c_c;&c_z;&c_a;&c_l;&c_ja;</trd>
  </trdgrp>
  <trd lng="bg">&c_t;&c_a;&c_n;&c_c;&c_u;&c_v;&c_a;&c_l;&c_n;&c_a;
  &c_z;&c_a;&c_l;&c_a;</trd>
