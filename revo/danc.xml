    <trd>&#x30C0;&#x30F3;&#x30B9;  <pr>&#x3060;&#x3093;&#x3059;</pr></trd>,
    <trd>&#x821E;&#x8E0A;  <pr>&#x3076;&#x3088;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">dans</trd>
  <trdgrp lng="pl">
    <trd>taniec</trd>,
    <trd>pl&aogonek;s</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_a;&c_n;&c_je;&c_c;</trd>,
    <trd>&c_p;&c_l;&c_ja;&c_s;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">tanec</trd>
  <trd lng="sv">dans</trd>
  <trd lng="th">&#xE01;&#xE32;&#xE23;&#xE40;&#xE15;&#xE49;&#xE19;&#xE23;&#xE33;</trd>
  <trd lng="tr">dans</trd>
  <trd lng="uk">&c_t;&c_a;&c_n;&c_je;&c_c;&c_mol;</trd>
  <trdgrp lng="zh">
    <trd>&#x821E; <pr>w&#x1D4;</pr></trd>,
    <trd>&#x821E;&#x8E48; <pr>w&#x1D4;d&#x1CE;o</pr></trd>,
    <trd>&#x8EA7;&#x6B65; <pr>x&#x1D0;b&#xF9;</pr></trd>
  </trdgrp>
