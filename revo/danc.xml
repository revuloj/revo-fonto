    <trd>Diskothek</trd>,
    <trd>Disko</trd>,
    <trd>Discothek</trd>,
    <trd>Disco</trd>,
    <trd>Ballsaal</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ballroom</trd>,
    <trd>dance hall</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>pista de baile</trd>,
    <trd>sal&oacute;n de baile</trd>,
    <trd>discoteca</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>dancing</trd>,
    <trd>discoth&egrave;que<klr tip="ind"> (dancing)</klr></trd>,
    <trd>bo&icirc;te<klr tip="ind"> (dancing)</klr></trd>
  </trdgrp>
  <trd lng="he">&#x5E8;&#x5D7;&#x5D1;&#x5EA; &#x5E8;&#x5D9;&#x5E7;&#x5D5;&#x5D3;&#x5D9;&#x5DD;</trd>
  <trdgrp lng="hu">
    <trd>t&aacute;nchelyis&eacute;g</trd>,
    <trd>t&aacute;ncterem</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>diskotek</trd>,
    <trd><ind>ruang</ind> dansa</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>discotheek</trd>,
    <trd>danszaal</trd>,
    <trd>dancing</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dancing</trd>,
    <trd>dyskoteka</trd>,
    <trd>sala <ind>balowa</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
