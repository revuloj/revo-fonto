    <trd>sal&oacute;</trd>,
    <trd>discoteca</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dancing</trd>,
    <trd>tan&ccaron;&iacute;rna</trd>
  </trdgrp>
  <trdgrp lng="de">
