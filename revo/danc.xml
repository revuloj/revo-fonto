<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>danc</rad>/i
</kap>
<drv mrk="danc.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Ritme movi la membrojn,  ordinare kun akompano de muziko:
      <ekz>
        <tld/>i anta&ubreve; iu sur piedoj k manoj
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>  
        <klr>(flategi iun)</klr>;
      </ekz>
      <ekz>
        <tld/>i valson <klr>(f)</klr>;
      </ekz>
      <ekz>
        la &scirc;ipeto <tld/>as sur la ondoj
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="alt">alti</ref>,
      <ref cel="balanc">balanci&gcirc;i</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">tan&ccaron;it</trd>
  <trd lng="de">tanzen</trd>
  <trd lng="en">dance</trd>
</drv>
<drv mrk="danc.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ritma movado de la membroj. 
    </dif>
    <refgrp tip="sin">
      <ref cel="polk.0o">polko</ref>,
      <ref cel="mazurk.0o">mazurko</ref>,
      <ref cel="kvadri.0o">kvadrilo</ref>,
      <ref cel="bal.0o">balo</ref>,
      <ref cel="bal.0eto">baleto</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Tanz</trd>
</drv>
<drv mrk="danc.0arto">
  <kap><tld/>arto</kap>
  <snc>
    <dif>
      Arto <tld/>i. 
    </dif>
  </snc>
  <trd lng="de">Tanzkunst</trd>
</drv>
<drv mrk="danc.sxnur0isto">
  <kap>&scirc;nur<tld/>isto</kap>
  <snc>
    <dif>
      Akrobato, kiu mar&scirc;as k <tld/>as sur stre&ccirc;ita &scirc;nuro.  
    </dif>
  </snc>
  <trd lng="de">Seilt&auml;nzer</trd>
</drv>
</art>
<!--
$Log$
-->
</vortaro>