<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>danc</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="danc.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="danc.0i.MUZ">
    <tezrad fak="MUZ"/>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Ritme movi la membrojn, ordinare kun akompano de muziko:
      <ekz>
        &scirc;i volas <tld/>i
        <fnt><bib>F</bib>, <lok>&FE; 20</lok></fnt>;
      </ekz>
      <ekz>
        kiam vi vidos, ke la knabinoj de &Scirc;ilo eliras, por <tld/>i
        en
        rondoj, tiam <klr>...</klr> kaptu al vi &ccirc;iu edzinon el la
        knabinoj
        <fnt><bib>MT</bib>, <lok>&Jug; 21:21</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj virinoj eliris post &scirc;i kun tamburinoj kaj
        <tld/>ante
        <fnt><bib>MT</bib>, <lok>&Eli; 15:20</lok></fnt>;
      </ekz>
      <ekz>
        kiam li alproksimi&gcirc;is al la tendaro kaj ekvidis la bovidon
        kaj la <tld/>adon, tiam ekflamis la kolero de Moseo
        <fnt><bib>MT</bib>, <lok>&Eli; 32:19</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i anta&ubreve; iu sur piedoj kaj manoj
        <klr>(flategi iun)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la &scirc;ipeto <tld/>as sur la ondoj.
      </ekz>
    </dif>
    <rim mrk="danc.0i.RIM_transitiva">
      La verbo estas netransitiva, sed akceptas nomon de <tld/>o kiel
      rektan objekton:
      <ekz>
        <tld/>i valson.
      </ekz>
    </rim>
    <refgrp tip="vid">
      <ref cel="bal.0o">balo</ref>,
      <ref cel="balanc1.0igxi">balanci&gcirc;i</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_n;&c_c;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_t;&c_a;&c_n;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_t;&c_a;&c_n;&c_c;&c_u;&c_v;&c_a;&c_m;</trd>
  <trdgrp lng="br">
    <trd>da&ntilde;sal</trd>,
    <trd>korolli&ntilde;</trd>
  </trdgrp>
  <trd lng="cs">tan&ccaron;it</trd>
  <trd lng="de">tanzen</trd>
  <trd lng="en">dance</trd>
  <trd lng="es">bailar</trd>
  <trdgrp lng="ca">
    <trd>ballar</trd>,
    <trd>dan&ccedil;ar</trd>
  </trdgrp>
  <trd lng="fr">danser</trd>
  <trd lng="hu">t&aacute;ncol</trd>
  <trd lng="nl">dansen</trd>
  <trdgrp lng="pl">
    <trd>ta&nacute;czy&cacute;</trd>,
    <trd>pl&aogonek;sa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>dan&ccedil;ar</trd>,
    <trd>bailar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_a;&c_n;&c_c;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_l;&c_ja;&c_s;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">tancova&tcaron;</trd>
  <trd lng="sv">dansa</trd>
  <trd lng="tr">dansetmek</trd>
</drv>

<drv mrk="danc.0o">
  <kap><tld/>o</kap>
  <snc mrk="danc.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Ritma movado de la membroj; aparta speco de tia okupo:
      <ekz>
        ili gloru Lian nomon en <tld/>o, per tamburino kaj harpo ili
        muziku al Li
        <fnt><bib>MT</bib>, <lok>&Psa; 149:3</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="balet.0o">baleto</ref>,
      <ref cel="gavot.0o.danco">gavoto</ref>,
      <ref cel="kvadri.0o">kvadrilo</ref>,
      <ref cel="mazurk.0o">mazurko</ref>,
      <ref cel="menuet.0o">menueto</ref>,
      <ref cel="polk.0o">polko</ref>,
      <ref cel="tang.0o">tango</ref>,
      <ref cel="vals.0o">valso</ref>.
    </refgrp>
  </snc>
  <adm>
    Indus kompletigi la liston de dancoj per io pli moderna... kun la
    koncernaj artikoloj.
    <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_n;&c_je;&c_c;</trd>,
    <trd>&c_s;&c_k;&c_o;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="bg">&c_t;&c_a;&c_n;&c_c;</trd>
  <trdgrp lng="br">
    <trd>da&ntilde;s</trd>,
    <trd>koroll</trd>
  </trdgrp>
  <trd lng="cs">tanec</trd>
  <trd lng="de">Tanz</trd>
  <trd lng="en">dance</trd>
  <trd lng="es">baile</trd>
  <trd lng="fr">danse<klr tip="ind"> (action, mani&egrave;re de
  danser)</klr></trd>
  <trd lng="hu">t&aacute;nc</trd>
  <trd lng="nl">dans</trd>
  <trdgrp lng="pl">
    <trd>taniec</trd>,
    <trd>pl&aogonek;s</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_a;&c_n;&c_je;&c_c;</trd>,
    <trd>&c_p;&c_l;&c_ja;&c_s;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">tanec</trd>
  <trd lng="sv">dans</trd>
  <trd lng="tr">dans</trd>
</drv>

<drv mrk="danc.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="danc.0ejo.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Loko a&ubreve; <ref tip="super"
      cel="konstr.0ajxo">konstrua&jcirc;o</ref> uzata
      por <tld/>i, kun ta&ubreve;ga areno:
      <ekz>
        sub&ccirc;iela <tld/>ejo;
      </ekz>
      <ekz>
        geja <tld/>ejo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_n;&c_c;&c_p;&c_l;&c_ja;&c_c;&c_o;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_d;&c_a;&c_n;&c_s;&c_ib;&c_n;&c_g;</trd>,
    <trd>&c_d;&c_y;&c_s;&c_k;&c_a;&c_t;&c_e;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_a;&c_n;&c_c;&c_z;&c_a;&c_l;&c_ja;</trd>
  </trdgrp>
  <trd lng="bg">&c_t;&c_a;&c_n;&c_c;&c_u;&c_v;&c_a;&c_l;&c_n;&c_a;
  &c_z;&c_a;&c_l;&c_a;</trd>
  <trdgrp lng="br">
    <trd>ti-da&ntilde;sal</trd>,
    <trd>korolldi</trd>
  </trdgrp>
  <trd lng="cs">tan&ccaron;&iacute;rna</trd>
  <trdgrp lng="de">
    <trd>Tanzsaal</trd>,
    <trd>Diskothek</trd>,
    <trd>Disko</trd>,
    <trd>Discothek</trd>,
    <trd>Disco</trd>,
    <trd>Ballsaal</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ballroom</trd>,
    <trd>dance hall</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>dancing</trd>,
    <trd>discoth&egrave;que<klr tip="ind"> (dancing)</klr></trd>,
    <trd>bo&icirc;te<klr tip="ind"> (dancing)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>t&aacute;nchelyis&eacute;g</trd>,
    <trd>t&aacute;ncterem</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>discotheek</trd>,
    <trd>danszaal</trd>,
    <trd>dancing</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dancing</trd>,
    <trd>dyskoteka</trd>,
    <trd>sala <ind>balowa</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_a;&c_n;&c_c;&c_p;&c_l;&c_o;&c_shch;&c_a;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_d;&c_a;&c_n;&c_s;&c_i;&c_n;&c_g;</trd>
  </trdgrp>
  <trd lng="tr">diskotek</trd>
</drv>

<drv mrk="danc.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="danc.0isto.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Iu, kiu profesie <tld/>as:
      <ekz>
        kiel majesta estis hodia&ubreve; la re&gcirc;o de Izrael, kiu
        elmontris sin <klr>...</klr>, kiel sin elmontras iu el la
        publikaj <tld/>istoj!
        <fnt><bib>MT</bib>, <lok>&Sa2; 6:20</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_n;&c_c;&c_o;&c_r;</trd>
  <trd lng="bg">&c_t;&c_a;&c_n;&c_c;&c_mol;&c_o;&c_r;</trd>
  <trdgrp lng="br">
    <trd>da&ntilde;ser</trd>,
    <trd>koroller</trd>
  </trdgrp>
  <trd lng="cs">tane&ccaron;n&iacute;k</trd>
  <trd lng="de">T&auml;nzer</trd>
  <trd lng="en">dancer</trd>
  <trdgrp lng="fr">
    <trd>danseur</trd>,
    <trd>danseuse</trd>
  </trdgrp>
  <trd lng="hu">t&aacute;ncos</trd>
  <trd lng="nl">danser</trd>
  <trd lng="pl">tancerz</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_a;&c_n;&c_c;&c_o;&c_v;&c_shch;&c_i;&c_k;</trd>,
    <trd>&c_p;&c_l;&c_ja;&c_s;&c_u;&c_n;</trd>
  </trdgrp>
</drv>

<drv mrk="danc.0istino">
  <kap><tld/>istino</kap>
  <snc mrk="danc.0istino.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Virino, kiu profesie <tld/>as:
      <ekz>
        en la subtegmenta &ccirc;ambreto staras duone
        vestita malgranda <ind><tld/>istino</ind>
        <fnt><bib>Fab2</bib>, <lok>Ne&gcirc;a re&gcirc;ino</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_a;&c_n;&c_c;&c_o;&c_r;&c_k;&c_a;</trd>
    <trd lng="cs">tane&ccaron;nice</trd>
    <trd lng="de">T&auml;nzerin</trd>
    <trd lng="hu">t&aacute;ncosn&odblac;</trd>
    <trd lng="nl">danseres</trd>
    <trd lng="pl">tancerka</trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_a;&c_n;&c_c;&c_o;&c_v;&c_shch;&c_i;&c_c;&c_a;</trd>,
      <trd>&c_p;&c_l;&c_ja;&c_s;&c_u;&c_n;&c_mol;&c_ja;</trd>,
      <trd>&c_b;&c_a;&c_l;&c_je;&c_r;&c_i;&c_n;&c_a;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="danc.klak0o">
  <kap>klak<tld/>o</kap>
  <snc mrk="danc.klak0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Usondevena <ref tip="super" cel="danc.0o.MUZ"><tld/>o</ref>, en 
      kiu oni klake frapas la piedojn sur la planko, populara precipe 
      en la unua duono de la 20a jarcento:
      <ekz>
        en la nokto a&ubreve;di&gcirc;is iliaj babiladoj, kantoj kaj
        klak<tld/>oj
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de
        Diego</aut>, <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_t;&c_a;&c_n;&c_c; &c_n;&c_a;
  &c_s;&c_t;&c_je;&c_p;</trd>
  <trd lng="de">Stepptanz</trd>
  <trd lng="en">tap dance</trd>
  <trd lng="fr">claquettes</trd>
  <trd lng="hu">sztepp</trd>
  <trd lng="it">tip-tap</trd>
  <trd lng="nl">stepdans</trd>
  <trd lng="pl">stepowanie</trd>
</drv>

<drv mrk="danc.sxnur0isto">
  <kap>&scirc;nur<tld/>isto</kap>
  <snc mrk="danc.sxnur0isto.BELA">
    <dif>
      Akrobato, kiu pa&scirc;as kaj <tld/>as sur stre&ccirc;ita
      &scirc;nuro.
    </dif>
  </snc>
  <trd
  lng="be">&c_k;&c_a;&c_n;&c_a;&c_t;&c_a;&c_h;&c_o;&c_d;&c_z;&c_je;&c_c;</trd>
  <trd
  lng="bg">&c_v;&c_malmol;&c_zh;&c_je;&c_i;&c_g;&c_r;&c_a;&c_ch;</trd>
  <trd lng="br">fungoroller</trd>
  <trd lng="de">Seilt&auml;nzer</trd>
  <trd lng="en">tightrope walker</trd>
  <trdgrp lng="fr">
    <trd>fildef&eacute;riste</trd>,
    <trd>funambule</trd>
  </trdgrp>
  <trd lng="hu">k&ouml;t&eacute;lt&aacute;ncos</trd>
  <trd lng="nl">koorddanser</trd>
  <trd
  lng="ru">&c_k;&c_a;&c_n;&c_a;&c_t;&c_o;&c_h;&c_o;&c_d;&c_je;&c_c;</trd>
</drv>
</art>
