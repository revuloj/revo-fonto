    <dif>
      Loko a&ubreve; <ref tip="super" cel="konstr.0ajxo">konstrua&jcirc;o</ref> uzata
      por <tld/>i, kun ta&ubreve;ga areno:
      <ekz>
        sub&ccirc;iela <tld/>ejo;
      </ekz>
      <ekz>
        geja <tld/>ejo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_n;&c_c;&c_p;&c_l;&c_ja;&c_c;&c_o;&c_w;&c_k;&c_a;</trd>,
