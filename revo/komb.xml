<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: komb.xml,v 1.38 2022/03/22 15:44:12 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>komb</rad>/i
</kap>

<drv mrk="komb.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malkonfuzi, purigi, ordigi per <tld/>ilo:
      <ekz>
        <tld/>i barbon, harojn;
      </ekz>
      <ekz>
        sin <tld/>i;
      </ekz>
      <ekz>
        <tld/>i hundon.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_ch;&c_a;&c_s;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="bg">
    <trd>&c_r;&c_je;&c_sh;&c_a;</trd>,
    <trd>&c_s;&c_r;&c_je;&c_s;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="bo">&#xF62;&#xF92;&#xFB1;&#xF74;&#xF42;&#xF0B;&#xF64;&#xF51;&#xF0B;&#xF62;&#xF92;&#xFB1;&#xF42;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>&ccaron;esat</trd>,
    <trd>&ccaron;esat <klr>(vlasy)</klr></trd>
  </trdgrp>
  <trd lng="de">k&auml;mmen</trd>
  <trd lng="en">comb</trd>
  <trd lng="es">peinar</trd>
  <trd lng="fr">peigner<klr tip="ind"> (cheveux, poils)</klr></trd>
  <trd lng="hu">f&eacute;s&uuml;l</trd>
  <trd lng="id"><baz>sisir</baz>menyisir</trd>
  <trd lng="nl">kammen</trd>
  <trd lng="pl">czesa&cacute;</trd> 
  <trdgrp lng="pt">
    <trd>pentear</trd>,
    <trd>cardar</trd>
  </trdgrp>
  <trd lng="ro">piept&abreve;na</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>u&ccaron;esa&tcaron;</trd>,
    <trd>&ccaron;esa&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x447;&#x435;&#x441;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x440;&#x438;&#x447;&#x456;&#x441;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x437;&#x430;&#x447;&#x456;&#x441;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="komb.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="komb.0ilo.ordigi">
    <dif>
      Ilo el korno, celuloido, metalo kc, kun longaj maldikaj dentoj por
      malkonfuzi, purigi, ordigi la harojn.
    </dif>
    <trd lng="be">&c_r;&c_a;&c_s;&c_ch;&c_o;&c_s;&c_k;&c_a;</trd>
    <trd lng="en">comb</trd>
    <trd lng="ru">&c_r;&c_a;&c_s;&c_ch;&c_jo;&c_s;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="komb.0ilo.ornami">
    <dif>
      Analoga ilo, kiun uzas la virinoj por fiksi a&ubreve; ornami sian
      hararon.
    </dif>
    <trdgrp lng="be">
      <trd>&c_g;&c_r;&c_e;&c_b;&c_je;&c_n;&c_mol;</trd>,
      <trd>&c_g;&c_r;&c_a;&c_b;&c_je;&c_n;&c_je;&c_c;</trd>, 
      <trd>&c_g;&c_r;&c_a;&c_b;&c_je;&c_n;&c_mol;&c_ch;&c_y;&c_k;</trd>
    </trdgrp>
    <trd lng="en">comb</trd>
  </snc>
  <trd lng="bg">&c_g;&c_r;&c_je;&c_b;&c_je;&c_n;</trd>
  <trd lng="bo">&#xF62;&#xF92;&#xFB1;&#xF74;&#xF42;&#xF0B;&#xF64;&#xF51;&#xF0B;</trd>
  <trd lng="cs">h&rcaron;eben na &ccaron;es&aacute;n&iacute;</trd>
  <trd lng="de">Kamm</trd>
  <trd lng="es">peine</trd>
  <trd lng="fr">peigne</trd>
  <trd lng="hu">f&eacute;s&udblac;</trd>
  <trd lng="id">sisir</trd>
  <trd lng="ku">&scedil;eh</trd>
  <trd lng="nl">kam</trd>
  <trd lng="pl">grzebie&nacute;</trd>     
  <trd lng="ro">pieptene </trd>
  <trd lng="ru">&c_g;&c_r;&c_je;&c_b;&c_je;&c_n;&c_mol;</trd>
  <trd lng="sk">hrebe&ncaron; <klr>(na &ccaron;esanie)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x433;&#x440;&#x435;&#x431;&#x456;&#x43D;&#x44C;</trd>,
    <trd>&#x433;&#x440;&#x435;&#x431;&#x456;&#x43D;&#x435;&#x446;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="komb.0isto">
  <kap>
    <tld/>isto
  </kap>
  <snc>
      <ref tip="dif" cel="barbir.0o">barbiro</ref>
  </snc>
  <trd lng="fr">coiffeur</trd>
  <trd lng="hu">fodr&aacute;sz</trd>
  <trd lng="nl">kapper</trd>
  <trd lng="pl">fryzjer</trd>     
  <trd lng="ro">frizer</trd>  
  <trd lng="uk">&#x43F;&#x435;&#x440;&#x443;&#x43A;&#x430;&#x440;</trd>
</drv>

<drv mrk="komb.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malordigi la <tld/>itajn harojn:
      <ekz>
        la vento vin mal<tld/>is.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_t;&c_r;&c_a;&c_p;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_u;&c_d;&c_l;&c_a;&c_c;&c_ib;&c_c;&c_mol;</trd>, 
    <trd>&c_p;&c_a;&c_k;&c_a;&c_l;&c_m;&c_a;&c_c;&c_ib;&c_c;&c_mol;</trd>, 
    <trd>&c_r;&c_a;&c_s;&c_k;&c_a;&c_l;&c_m;&c_a;&c_c;&c_ib;&c_c;&c_mol;</trd>, 
    <trd>&c_u;&c_z;&c_l;&c_a;&c_h;&c_m;&c_a;&c_c;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>cuchat</trd>,
    <trd>rozcuchat</trd>
  </trdgrp>
  <trd lng="de">verstrubbeln</trd>
  <trdgrp lng="en">
    <trd>tousle</trd>,
    <trd>dishevel</trd>
  </trdgrp>
  <trd lng="es">despeinar</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;coiffer</trd>,
    <trd>d&eacute;peigner</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&ouml;sszek&oacute;col</trd>,
    <trd>sz&eacute;tborzol</trd>
  </trdgrp>
  <trd lng="nl">in de <ind>war</ind> doen</trd>
  <trdgrp lng="pl">
    <trd>targa&cacute;</trd>,
    <trd>potarga&cacute;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>toart&abreve;</trd>,
    <trd> sf&acirc;&#x219;ia</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_t;&c_r;&c_je;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_l;&c_o;&c_h;&c_m;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">postrapati&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x440;&#x43E;&#x437;&#x442;&#x440;&#x456;&#x43F;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x440;&#x43E;&#x437;&#x43A;&#x443;&#x439;&#x43E;&#x432;&#x434;&#x436;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x441;&#x43A;&#x443;&#x439;&#x43E;&#x432;&#x434;&#x436;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="komb.dis0i">
  <kap>dis<tld/>i</kap>
  <snc>
  <uzo tip="fak">TEK</uzo>
    <dif>
      <tld lit="K"/>i per speciala <tld/>ilo linajn a&ubreve; kanabajn fibrojn
      por purigi ilin.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_s;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_ch;&c_e;&c_s;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">ausk&auml;mmen</trd>
  <trd lng="en">card</trd>
  <trdgrp lng="fr">
    <trd>carder</trd>,
    <trd>peigner<klr tip="ind"> (carder)</klr></trd>,
    <trd>d&eacute;m&ecirc;ler<klr tip="ind"> (carder)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kif&eacute;s&uuml;l</trd>,
    <trd>k&aacute;rtol</trd>,
    <trd>gerebenez</trd>
  </trdgrp>
  <trd lng="nl">kaarden</trd>
  <trdgrp lng="pl">
    <trd>przeczesywa&cacute;</trd>,
    <trd>rozczesa&cacute;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>piept&abreve;na</trd>,
    <trd>perie</trd>
  </trdgrp>
  <trd lng="uk">&#x440;&#x43E;&#x437;&#x447;&#x456;&#x441;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
</drv>

<drv mrk="komb.diraka0ilo">
  <kap>diraka <tld/>ilo</kap>
  <snc>
  <uzo tip="fak">MAT</uzo>
    <dif>
      <klr>(kun periodo T) </klr>
      <ref tip="super" cel="distri2.0o">Distribucio</ref>, kiu &jcirc;etas
      funkcion <frm am="phi">&phi;</frm> al
      <frm am="sum_(n in bb Z) phi(nT)">&#x2211;<sub><k>n</k>&#x2208;<g>Z</g></sub> &phi;(<k>n</k>T)</frm>:
      <ekz>
        la dirakan <tld/>ilon oni kutime signas 
        per <frm am="&c_Sh;_T">&c_Sh;<sub>T</sub></frm> <klr>(legu: &scirc;a to)</klr>;
      </ekz>
      <ekz>
        la furiera transformo de diraka <tld/>ilo estas
        diraka <tld/>ilo.
      </ekz>
    </dif>
    <ref tip="vid" cel="dirak.0o">Dirako</ref>
  </snc>
  <trd lng="de">Diracscher <ind>Kamm</ind></trd>
  <trd lng="en">Dirac['s] <ind>comb</ind></trd>
  <trd lng="es">peine de Dirac</trd>
  <trd lng="fr">peigne de Dirac</trd>
  <trdgrp lng="hu">
    <trd>Dirac-sorozat</trd>,
    <trd>f&eacute;s&udblac;f&uuml;ggv&eacute;ny</trd>
  </trdgrp>
  <trd lng="nl">kam van Dirac</trd>
  <trd lng="pl">grzebie&nacute; Diraca <klr>(mat.)</klr></trd>
  <trd lng="ro">Dirac pieptene</trd>
  <trd lng="ru">&c_Sh;-&c_f;&c_u;&c_n;&c_k;&c_c;&c_i;&c_ja;</trd>
</drv>
</art>
