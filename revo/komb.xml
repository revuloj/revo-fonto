<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>komb</rad>/i
</kap>
<drv mrk="komb.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malkonfuzi, purigi, ordigi per <tld/>ilo:
      <ekz>
        <tld/>i barbon, harojn;
      </ekz>
      <ekz>
        sin <tld/>i;
      </ekz>
      <ekz>
        <tld/>i hundon.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">&ccaron;esat</trd>
  <trd lng="de">k&auml;mmen</trd>
  <trd lng="en">comb</trd>
  <trd lng="fr">peigner<klr tip="ind"> (cheveux, poils)</klr></trd>
</drv>
<drv mrk="komb.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Ilo el korno, celuloido, metalo kc, kun longaj maldikaj dentoj por 
      malkonfuzi, purigi, ordigi la harojn.
    </dif>
    <trd>lng="en">comb</trd>
  </snc>
  <snc>
    <dif>
      Analoga ilo, kiun uzas la virinoj por fiksi a&ubreve; ornami sian hararon.
    </dif>
    <trd>lng="en">comb</trd>
  </snc>
  <trd lng="de">Kamm</trd>
  <trd lng="fr">peigne</trd>
</drv>
<drv mrk="komb.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malordigi la <tld/>itajn harojn:
      <ekz>
        la vento vin mal<tld/>is.
      </ekz>
    </dif>
    <trd lng="de">verstrubbeln</trd>
    <trdgrp lng="en">
      <trd>tousle</trd>,
      <trd>dishevel</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;coiffer</trd>,
      <trd>d&eacute;peigner</trd></trdgrp> 
  </snc>
</drv>
<drv mrk="komb.dis0i">
  <kap>dis<tld/>i</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      <tld lit="K"/>i per speciala <tld/>ilo linajn a&ubreve; kanabajn fibrojn
      por purigi ilin.
    </dif>
    <trd lng="de">ausk&auml;mmen</trd>
    <trd lng="en">card</trd>
    <trdgrp lng="fr">
      <trd>carder</trd>,
      <trd>peigner<klr tip="ind"> (carder)</klr></trd>,
      <trd>d&eacute;m&ecirc;ler<klr tip="ind"> (carder)</klr></trd></trdgrp> 
  </snc>
</drv>
<drv mrk="komb.diraka0ilo">
  <kap>diraka <tld/>ilo</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(kun periodo T)</klr>
      <ref tip="super" cel="distri2.0o">Distribucio</ref>, kiu &jcirc;etas funkcion &phi; al 
      &#x2211;<sub>n&#x2208;Z</sub>&#xa0;&phi;(nT):
      <ekz>
        la dirakan <tld/>ilon oni kutime signas per &c_Sh;<sub>T</sub>
        <klr>(legu &scirc;a to)</klr>;
      </ekz>
      <ekz>
        la furiera transformo de diraka <tld/>ilo estas
        diraka <tld/>ilo.
      </ekz>
    </dif>
    <ref tip="vid" cel="dirak.0o">Dirako.</ref>
  </snc>
  <trd lng="fr">peigne de Dirac</trd>
</drv>

</art>
