<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>komb</rad>/i
</kap>
<drv mrk="komb.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malkonfuzi, purigi, ordigi per <tld/>ilo:
      <ekz>
        <tld/>i barbon, harojn;
      </ekz>
      <ekz>
        sin <tld/>i;
      </ekz>
      <ekz>
        <tld/>i hundon.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">&ccaron;esat</trd>
  <trd lng="de">k&auml;mmen</trd>
  <trd lng="en">comb</trd>
  <trd lng="es">peinar</trd>
  <trd lng="fr">peigner<klr tip="ind"> (cheveux, poils)</klr></trd>
  <trd lng="nl">kammen</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="komb.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="komb.0ilo.ordigi">
    <dif>
      Ilo el korno, celuloido, metalo kc, kun longaj maldikaj dentoj por
      malkonfuzi, purigi, ordigi la harojn.
    </dif>
    <trd lng="en">comb</trd>
    <trd lng="ru">&c_r;&c_a;&c_s;&c_ch;&c_jo;&c_s;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="komb.0ilo.ornami">
    <dif>
      Analoga ilo, kiun uzas la virinoj por fiksi a&ubreve; ornami sian hararon.
    </dif>
    <trd>lng="en">comb</trd>
  </snc>
  <trd lng="de">Kamm</trd>
  <trd lng="es">peine</trd>
  <trd lng="fr">peigne</trd>
  <trd lng="nl">kam</trd>
  <trd lng="ru">&c_g;&c_r;&c_je;&c_b;&c_je;&c_n;&c_mol;</trd>
</drv>

<drv mrk="komb.0isto">
  <kap>
    <tld/>isto
  </kap>
  <snc>
    <dif>
      <ref tip="dif" cel="barbir.0o">Barbiro</ref>.
    </dif>
  </snc>
</drv>

<drv mrk="komb.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malordigi la <tld/>itajn harojn:
      <ekz>
        la vento vin mal<tld/>is.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">verstrubbeln</trd>
  <trdgrp lng="en">
    <trd>tousle</trd>,
    <trd>dishevel</trd>
  </trdgrp>
  <trd lng="es">despeinar</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;coiffer</trd>,
    <trd>d&eacute;peigner</trd>
  </trdgrp>
  <trd lng="nl">in de <ind>war</ind> doen</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_t;&c_r;&c_je;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_l;&c_o;&c_h;&c_m;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="komb.dis0i">
  <kap>dis<tld/>i</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      <tld lit="K"/>i per speciala <tld/>ilo linajn a&ubreve; kanabajn fibrojn
      por purigi ilin.
    </dif>
  </snc>
  <trd lng="de">ausk&auml;mmen</trd>
  <trd lng="en">card</trd>
  <trdgrp lng="fr">
    <trd>carder</trd>,
    <trd>peigner<klr tip="ind"> (carder)</klr></trd>,
    <trd>d&eacute;m&ecirc;ler<klr tip="ind"> (carder)</klr></trd>
  </trdgrp>
  <trd lng="nl">kaarden</trd>
</drv>

<drv mrk="komb.diraka0ilo">
  <kap>diraka <tld/>ilo</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(kun periodo T)</klr>
      <ref tip="super" cel="distri2.0o">Distribucio</ref>, kiu &jcirc;etas
      funkcion <frm>&phi;</frm> al
      <frm>&#8721;<sub><k>n</k>&#8712;<g>Z</g></sub> &phi;(<k>n</k>T)</frm>:
      <ekz>
        la dirakan <tld/>ilon oni kutime signas 
        per <frm>&c_Sh;<sub>T</sub></frm> <klr>(legu: &scirc;a to)</klr>;
      </ekz>
      <ekz>
        la furiera transformo de diraka <tld/>ilo estas
        diraka <tld/>ilo.
      </ekz>
    </dif>
    <ref tip="vid" cel="dirak.0o">Dirako</ref>
  </snc>
  <trd lng="de">Diracscher <ind>Kamm</ind></trd>
  <trd lng="en">Dirac['s] <ind>comb</ind></trd>
  <trd lng="es">peine de Dirac</trd>
  <trd lng="fr">peigne de Dirac</trd>
  <trd lng="nl">kam van Dirac</trd>
  <trd lng="pl">grzebie&nacute; Diraca</trd>
  <trd lng="ru">&c_Sh;-&c_f;&c_u;&c_n;&c_k;&c_c;&c_i;&c_ja;</trd>
</drv>
</art>
