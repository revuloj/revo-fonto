    <trd>&c_s;&c_d;&c_je;&c_r;&c_zh;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">mierny</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43C;&c_ib;&c_r;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43F;&#x43E;&#x43C;&c_ib;&c_r;&#x43A;&#x43E;&c_v;&c_a;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_i;&#x43C;&c_a;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_s;&#x43A;&c_r;&#x43E;&#x43C;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="moder.0eco">
  <kap><tld/>eco, <var><kap><tld/>o</kap></var></kap>
  <snc mrk="moder.0eco.KOMUNE">
    <dif>
      Eco de iu a&ubreve; io <tld/>a:
      <ekz>
        en la mezo de la <klr>[&#8230;]</klr> uragano de via pasio vi devas observi certan
        <tld/>econ, kiu donas al &ccirc;io harmonion
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        sa&gcirc;econ, <tld/>econ, paciencon ne konis ili
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        li sciis gardi certan <klr>[&#8230;]</klr> <tld/>econ en dibo&ccirc;o
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        vivo sen <tld/>o kondukas al mizero
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_r;&c_y;&c_m;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_ja;&c_r;&c_k;&c_o;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_u;&c_m;&c_je;&c_r;&c_je;&c_n;&c_o;&c_s;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>m&iacute;rnost</trd>,
    <trd>zdr&zcaron;enlivost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>M&auml;&szlig;igung</trd>,
    <trd>M&auml;&szlig;igkeit</trd>
  </trdgrp>
  <trd lng="en">moderation</trd>
  <trd lng="es">moderaci&oacute;n</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&f_y;&a_A;&a_n;&a_h;&#8204;&a_r;&a_w;&f_y;</trd>,
    <trd>&a_A;&a_ayn;&a_t;&a_d;&a_A;&a_l;</trd>
  </trdgrp>
  <trd lng="fr">mod&eacute;ration</trd>
  <trdgrp lng="hu">
    <trd>m&eacute;rs&eacute;kletess&eacute;g</trd>,
    <trd>m&eacute;rt&eacute;kletess&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#36969;&#x5EA6;  <pr>&#12390;&#x304D;&#12393;</pr></trd>,
    <trd>&#x7BC0;&#x5EA6;  <pr>&#x305B;&#12388;&#12393;</pr></trd>,
    <trd>&#x4E2D;&#x5EB8;  <pr>&#12385;&#12421;&#12358;&#12424;&#12358;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gematigdheid</trd>,
    <trd>mate</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>skromno&sacute;&cacute;</trd>,
    <trd>umiarkowanie</trd>
  </trdgrp>
  <trd lng="ro">cump&abreve;tare</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_m;&c_je;&c_r;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_d;&c_je;&c_r;&c_zh;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>miernos&tcaron;</trd>,
    <trd>umiernenos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43C;&c_ib;&c_r;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&#x43F;&#x43E;&#x43C;&c_ib;&c_r;&#x43A;&#x43E;&c_v;&c_a;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_i;&#x43C;&c_a;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&c_s;&#x43A;&c_r;&#x43E;&#x43C;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="moder.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="moder.0igi.KOMUNE">
    <dif>
      Igi ion a&ubreve; iun <tld/>a:
      <ekz>
        kolerego malfacile <tld/>igita kuntiris liajn lipojn
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        la amo <klr>[&#8230;]</klr> naski&gcirc;as per tempo, kaj la sperto anka&ubreve; montras, ke tempo
        &gcirc;in tre ofte <tld/>igas
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        por &scirc;afo tondita Dio venton <tld/>igas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="brid.0i.FIG">bridi<sncref/></ref>,
      <ref cel="mild.0igi">mildigi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_r;&c_y;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_mol;&c_c;&c_ib;&c_sh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_r;&c_a;&c_v;&c_ja; &c_u;&c_m;&c_je;&c_r;&c_je;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>m&iacute;rnit <klr>(rychlost)</klr></trd>,
    <trd>zm&iacute;rnit</trd>,
    <trd>zm&iacute;r&ncaron;ovat</trd>
  </trdgrp>
  <trd lng="de">m&auml;&szlig;igen</trd>
  <trd lng="en">moderate</trd>
  <trd lng="es">moderar</trd>
  <trd lng="fa">&a_t;&a_ayn;&a_d;&f_y;&a_l; &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>mod&eacute;rer</trd>,
    <trd>temp&eacute;rer</trd>
  </trdgrp>
  <trd lng="hu">m&eacute;rs&eacute;kel</trd>
  <trdgrp lng="ja">
    <trd>&#x7BC0;&#21046;&#12377;&#x308B;  <pr>&#x305B;&#12387;&#x305B;&#12356;&#12377;&#x308B;</pr></trd>,
    <trd>&#x7DE9;&#x548C;&#12377;&#x308B;  <pr>&#x304B;&#12435;&#x308F;&#12377;&#x308B;</pr></trd>,
    <trd>&#25233;&#12360;&#x308B;  <pr>&#x304A;&#12373;&#12360;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">matigen</trd>
  <trdgrp lng="pl">
    <trd>moderowa&cacute;</trd>,
    <trd>poskramia&cacute;</trd>,
    <trd>za&lstroke;agodzi&cacute;</trd>,
    <trd>przyt&lstroke;umia&cacute;</trd>,
    <trd>u&sacute;mierza&cacute;</trd>
  </trdgrp>
    <trdgrp lng="ro">
    <trd>modera</trd>,
    <trd>&icirc;mbl&acirc;nzi</trd>,
    <trd>cump&abreve;ta</trd>,
    <trd>suprim&abreve;</trd>,
    <trd>calmeaz&abreve;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_m;&c_je;&c_r;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_d;&c_je;&c_r;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zmierni&tcaron;</trd>
  <trd lng="uk">&c_z;&#x43C;&c_je;&#x43D;&c_sh;&c_i;&c_t;&c_i;</trd>
</drv>

<drv mrk="moder.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="moder.0ismo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Pli&scirc;ato de <ref tip="vid" cel="kompro.0o">kompromisaj</ref>,
      maldrastaj, prudentaj agmanieroj, kontraste al <ref tip="ant"
      cel="ekstre.0ismo.POL">ekstremismo</ref>:
      <ekz>
        troigo en sociaj ekstremoj nomi&gcirc;as emfaze ekstremismo
        <klr>[&#8230;]</klr>, kies inverso estas <tld/>ismo
        <fnt>
          <aut>E. Pauli</aut>,
          <vrk><url ref="http://www.cfh.ufsc.br/~simpozio/">
          Enciklopedio simpozio</url></vrk>,
          <lok>artikolo &leftquot;ekstrema&rightquot;, 1996</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ismo do estas: malekstrema kaj neideologia pozicio
        <fnt>
          <aut>K. Miner</aut>,
          <vrk><url ref="http://www.sunflower.com/~miner/TAGLIBRO_package/taglibro.html">
          taglibro</url></vrk>,
          <lok>2004-03-24</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="defetism.0o">defetismo</ref>,
      <ref cel="konfor.0ismo">konformismo</ref>,
      <ref cel="oportu.0ismo">oportunismo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_e;&c_n;&c_t;&c_r;&c_y;&c_z;&c_m; <klr>(&c_u; &c_p;&c_a;&c_l;&c_ib;&c_t;&c_y;&c_c;&c_y;)</klr></trd>,
    <trd>&c_p;&c_a;&c_m;&c_ja;&c_r;&c_k;&c_o;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">Mitte <klr>(politisch)</klr></trd>
  <trdgrp lng="fr">
    <trd>centrisme</trd>,
    <trd>mesure<klr> (mod&eacute;ration)</klr></trd>,
    <trd>mod&eacute;ration</trd>
  </trdgrp>
  <trd lng="ja">&#x7A4F;&#x548C;&#x4E3B;&#x7FA9;  <pr>&#x304A;&#12435;&#x308F;&#12375;&#12421;&#x304E;</pr></trd>
  <trdgrp lng="pl">
    <trd>umiar</trd>,
    <trd>pow&sacute;ci&aogonek;gliwo&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>modera&#x21B;ie</trd>,
    <trd>cump&abreve;tare</trd>
  </trdgrp>
</drv>

<drv mrk="moder.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="moder.mal0a.KOMUNE">
    <dif>
      <ref tip="super" cel="tro.0a">Troa</ref>, ekscesa, ekstrema:
      <ekz>
        &scirc;i perdis la konscion &ccirc;e la penso, ke &scirc;i estas en la manoj de tiel mal<tld/>a
        kaj potenca sinjoro
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        knabino <klr>[&#8230;]</klr> meze de la mal<tld/>a kaj senbrida amaso
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kun karaktero tia, kia estas la &scirc;ia, pasia, mal<tld/>a, kaj pu&scirc;anta &ccirc;ion &gcirc;is
        ekstremo, mi konfesas, ke mi ne estas trankvila
        <fnt>
          <bib>ChL</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="brid.sen0a">senbrida</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="lim.0o">limo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_ja;&c_s;&c_t;&c_r;&c_y;&c_m;&c_a;&c_n;&c_y;</trd>
  <trdgrp lng="bg">
    <trd>&c_p;&c_r;&c_je;&c_k;&c_o;&c_m;&c_je;&c_r;&c_je;&c_n;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_k;&c_a;&c_l;&c_je;&c_n;</trd>,
    <trd>&c_n;&c_je;&c_u;&c_m;&c_je;&c_r;&c_je;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nest&rcaron;&iacute;dm&yacute;</trd>,
    <trd>nez&rcaron;&iacute;zen&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>unm&auml;&szlig;ig</trd>,
    <trd>exzessiv</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>excessive</trd>,
    <trd>immoderate</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>desmesurado</trd>,
    <trd>excesivo</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_A;&a_f;&a_r;&a_A;&a_T;&f_y;</trd>,
    <trd>&a_t;&a_n;&a_d;&a_r;&a_w;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>excessif</trd>,
    <trd>immod&eacute;r&eacute;</trd>,
    <trd>outr&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>m&eacute;rt&eacute;ktelen</trd>,
    <trd>szertelen</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x904E;&#x5EA6;&#x306E;  <pr>&#x304B;&#12393;&#x306E;</pr></trd>,
    <trd>&#x7BC0;&#x5EA6;&#x306E;&#x306A;&#12356;  <pr>&#x305B;&#12388;&#12393;&#x306E;&#x306A;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>onmatig</trd>,
    <trd>buitensporig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ekstremalny</trd>,
    <trd>przesadny</trd>,
    <trd>nieumiarkowany</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>extrem</trd>,
    <trd>exagerat</trd>,
    <trd>imoderat</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_je;&c_u;&c_m;&c_je;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>bezuzdn&yacute;</trd>,
    <trd>nestriedmy</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_je;&#x43F;&#x43E;&#x43C;&c_ib;&c_r;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43D;&c_je;&#x43F;&#x43E;&#x43C;&c_ib;&c_r;&#x43A;&#x43E;&c_v;&c_a;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43D;&c_je;&c_s;&c_t;&c_r;&c_i;&#x43C;&c_a;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
