<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>moder</rad>/a
</kap>
<drv mrk="moder.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Ne troa, ne ekscesa, ne ekstrema:
      <ekz>
        <tld/>a prezo, postulo;
      </ekz>
      <ekz>
        <tld/>a politika partio;
      </ekz>
      <ekz>
        vivo <tld/>a estas sendan&gcirc;era<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li &ccirc;iam restis <tld/>a en siaj ambicioj;
      </ekz>
      <ekz>
        <tld/>e man&gcirc;i.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="mez.0a">meza</ref>,
      <ref cel="modest.0a">modesta</ref>,
      <ref cel="konten.0a">kontenta</ref>,
      <ref cel="sobr.0a">sobra</ref>,
      <ref cel="mezur.0o">mezuro</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_r;&c_y;&c_m;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_ja;&c_r;&c_k;&c_o;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="en">moderate</trd>
  <trd lng="es">moderado, -a</trd>
  <trdgrp lng="hu">
    <trd>m&eacute;rs&eacute;kelt</trd>,
    <trd>m&eacute;rt&eacute;kletes</trd>,
    <trd>m&eacute;rt&eacute;ktart&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>matig</trd>,
    <trd>gematigd</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_m;&c_je;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_d;&c_je;&c_r;&c_zh;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="moder.0eco">
  <kap><tld/>(ec)o</kap>
  <snc>
    <dif>
      Eco de iu a&ubreve; io <tld/>a:
      <ekz>vivo sen <tld/>o kondukas al mizero
<fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_r;&c_y;&c_m;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_ja;&c_r;&c_k;&c_o;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>m&eacute;rs&eacute;kletess&eacute;g</trd>,
    <trd>m&eacute;rt&eacute;kletess&eacute;g</trd>
  </trdgrp>
  <trd lng="nl">gematigdheid</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_m;&c_je;&c_r;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_d;&c_je;&c_r;&c_zh;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="es">moderaci&oacute;n</trd>
</drv>

<drv mrk="moder.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi ion a&ubreve; iun <tld/>a:
      <ekz>
        <tld/>igi la rapidecon de a&ubreve;tomobilo, la ardon de la diskuto;
      </ekz>
      <ekz>por &scirc;afo tondita Dio venton <tld/>igas
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        <tld/>igi punon;
      </ekz>
      <ekz>
        <tld/>igi ies koleron
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="mild.0igi">mildigi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_r;&c_y;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_mol;&c_c;&c_ib;&c_sh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="hu">m&eacute;rs&eacute;kel</trd>  
  <trd lng="nl">matigen</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_m;&c_je;&c_r;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_d;&c_je;&c_r;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="es">moderar</trd>
</drv>

<drv mrk="moder.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Troa, ekscesa, ekstrema
    </dif>
    <refgrp tip="sin">
      <ref cel="brid.sen0a">senbrida</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="lim.0o">limo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_ja;&c_s;&c_t;&c_r;&c_y;&c_m;&c_a;&c_n;&c_y;</trd>
  <trdgrp lng="hu">
    <trd>m&eacute;rt&eacute;ktelen</trd>,
    <trd>szertelen</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>onmatig</trd>,
    <trd>buitensporig</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>desmesurado</trd>,
    <trd>excesivo</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_je;&c_u;&c_m;&c_je;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
