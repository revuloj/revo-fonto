<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: moder.xml,v 1.19 2021/02/14 19:10:32 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>moder</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="moder.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="moder.0a.KOMUNE">
    <dif>
      Ne troa, ne ekscesa, ne ekstrema:
      <ekz>
        <tld/>a prezo, postulo;
      </ekz>
      <ekz>
        <tld/>a politika partio;
      </ekz>
      <ekz>
        vivo <tld/>a estas sendan&gcirc;era<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li &ccirc;iam restis <tld/>a en siaj ambicioj;
      </ekz>
      <ekz>
        <tld/>e man&gcirc;i.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mez.0a">meza</ref>,
      <ref cel="modest.0a">modesta</ref>,
      <ref cel="sobr.0a">sobra</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_r;&c_y;&c_m;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_ja;&c_r;&c_k;&c_o;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_u;&c_m;&c_je;&c_r;&c_je;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>decentn&iacute;</trd>,
    <trd>m&iacute;rn&yacute;</trd>,
    <trd>um&iacute;rn&ecaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>m&auml;&szlig;ig</trd>,
    <trd>gem&auml;&szlig;igt</trd>
  </trdgrp>
  <trd lng="en">moderate</trd>
  <trd lng="es">moderado, -a</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&f_y;&a_A;&a_n;&a_h;&zwnj;&a_r;&a_w;</trd>,
    <trd>&a_m;&a_ayn;&a_t;&a_d;&a_l;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>m&eacute;diocre</trd>,
    <trd>mod&eacute;r&eacute;</trd>,
    <trd>modique</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>m&eacute;rs&eacute;kelt</trd>,
    <trd>m&eacute;rt&eacute;kletes</trd>,
    <trd>m&eacute;rt&eacute;ktart&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>matig</trd>,
    <trd>gematigd</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>skromny</trd>,
    <trd>opanowany</trd>,
    <trd>umiarkowany</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>moderado</trd>,
    <trd>comedido</trd>,
    <trd>frugal</trd>,
    <trd>m&oacute;dico</trd>,
    <trd>temperado <klr>(moderado)</klr></trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>modest</trd>,
    <trd>senin</trd>,
    <trd>moderat</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_m;&c_je;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_d;&c_je;&c_r;&c_zh;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">mierny</trd>
</drv>

<drv mrk="moder.0eco">
  <kap><tld/>eco, <var><kap><tld/>o</kap></var></kap>
  <snc mrk="moder.0eco.KOMUNE">
    <dif>
      Eco de iu a&ubreve; io <tld/>a:
      <ekz>
        vivo sen <tld/>o kondukas al mizero
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_r;&c_y;&c_m;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_ja;&c_r;&c_k;&c_o;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_u;&c_m;&c_je;&c_r;&c_je;&c_n;&c_o;&c_s;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>m&iacute;rnost</trd>,
    <trd>zdr&zcaron;enlivost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>M&auml;&szlig;igung</trd>,
    <trd>M&auml;&szlig;igkeit</trd>
  </trdgrp>
  <trd lng="en">moderation</trd>
  <trd lng="es">moderaci&oacute;n</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&f_y;&a_A;&a_n;&a_h;&zwnj;&a_r;&a_w;&f_y;</trd>,
    <trd>&a_A;&a_ayn;&a_t;&a_d;&a_A;&a_l;</trd>
  </trdgrp>
  <trd lng="fr">mod&eacute;ration</trd>
  <trdgrp lng="hu">
    <trd>m&eacute;rs&eacute;kletess&eacute;g</trd>,
    <trd>m&eacute;rt&eacute;kletess&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gematigdheid</trd>,
    <trd>mate</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>skromno&sacute;&cacute;</trd>,
    <trd>umiarkowanie</trd>
  </trdgrp>
  <trd lng="ro">cump&abreve;tare</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_m;&c_je;&c_r;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_d;&c_je;&c_r;&c_zh;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>miernos&tcaron;</trd>,
    <trd>umiernenos&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="moder.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="moder.0igi.KOMUNE">
    <dif>
      Igi ion a&ubreve; iun <tld/>a:
      <ekz>
        <tld/>igi la rapidecon de a&ubreve;tomobilo, la ardon de la diskuto;
      </ekz>
      <ekz>
        por &scirc;afo tondita Dio venton <tld/>igas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>igi punon;
      </ekz>
      <ekz>
        <tld/>igi ies koleron.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mild.0igi">mildigi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_r;&c_y;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_mol;&c_c;&c_ib;&c_sh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_r;&c_a;&c_v;&c_ja; &c_u;&c_m;&c_je;&c_r;&c_je;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>m&iacute;rnit <klr>(rychlost)</klr></trd>,
    <trd>zm&iacute;rnit</trd>,
    <trd>zm&iacute;r&ncaron;ovat</trd>
  </trdgrp>
  <trd lng="de">m&auml;&szlig;igen</trd>
  <trd lng="en">moderate</trd>
  <trd lng="es">moderar</trd>
  <trd lng="fa">&a_t;&a_ayn;&a_d;&f_y;&a_l; &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>mod&eacute;rer</trd>,
    <trd>temp&eacute;rer</trd>
  </trdgrp>
  <trd lng="hu">m&eacute;rs&eacute;kel</trd>
  <trd lng="nl">matigen</trd>
  <trdgrp lng="pl">
    <trd>moderowa&cacute;</trd>,
    <trd>poskramia&cacute;</trd>,
    <trd>za&lstroke;agodzi&cacute;</trd>,
    <trd>przyt&lstroke;umia&cacute;</trd>,
    <trd>u&sacute;mierza&cacute;</trd>
  </trdgrp>
    <trdgrp lng="ro">
    <trd>modera</trd>,
    <trd>&icirc;mbl&acirc;nzi</trd>,
    <trd>cump&abreve;ta</trd>,
    <trd>suprim&abreve;</trd>,
    <trd>calmeaz&abreve;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_m;&c_je;&c_r;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_d;&c_je;&c_r;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zmierni&tcaron;</trd>
</drv>

<drv mrk="moder.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="moder.0ismo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Pli&scirc;ato de <ref tip="vid" cel="kompro.0o">kompromisaj</ref>,
      maldrastaj, prudentaj agmanieroj, kontraste al <ref tip="ant"
      cel="ekstre.0ismo.POL">ekstremismo</ref>:
      <ekz>
        troigo en sociaj ekstremoj nomi&gcirc;as emfaze ekstremismo
        <klr>...</klr>, kies inverso estas <tld/>ismo
        <fnt>
          <aut>E. Pauli</aut>,
          <vrk><url ref="http://www.cfh.ufsc.br/~simpozio/">
          Enciklopedio simpozio</url></vrk>,
          <lok>artikolo &leftquot;ekstrema&rightquot;, 1996</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ismo do estas: malekstrema kaj neideologia pozicio
        <fnt>
          <aut>K. Miner</aut>,
          <vrk><url
ref="http://www.sunflower.com/~miner/TAGLIBRO_package/taglibro.html">
          taglibro</url></vrk>,
          <lok>2004-03-24</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>centrisme</trd>,
    <trd>mesure<klr> (mod&eacute;ration)</klr></trd>,
    <trd>mod&eacute;ration</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>umiar</trd>,
    <trd>pow&sacute;ci&aogonek;gliwo&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>modera&#539;ie</trd>,
    <trd>cump&abreve;tare</trd>
  </trdgrp>
</drv>

<drv mrk="moder.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="moder.mal0a.KOMUNE">
    <dif>
      Troa, ekscesa, ekstrema.
    </dif>
    <refgrp tip="sin">
      <ref cel="brid.sen0a">senbrida</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="lim.0o">limo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_ja;&c_s;&c_t;&c_r;&c_y;&c_m;&c_a;&c_n;&c_y;</trd>
  <trdgrp lng="bg">
    <trd>&c_p;&c_r;&c_je;&c_k;&c_o;&c_m;&c_je;&c_r;&c_je;&c_n;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_k;&c_a;&c_l;&c_je;&c_n;</trd>,
    <trd>&c_n;&c_je;&c_u;&c_m;&c_je;&c_r;&c_je;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nest&rcaron;&iacute;dm&yacute;</trd>,
    <trd>nez&rcaron;&iacute;zen&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>unm&auml;&szlig;ig</trd>,
    <trd>exzessiv</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>excessive</trd>,
    <trd>immoderate</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>desmesurado</trd>,
    <trd>excesivo</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_A;&a_f;&a_r;&a_A;&a_T;&f_y;</trd>,
    <trd>&a_t;&a_n;&a_d;&a_r;&a_w;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>excessif</trd>,
    <trd>immod&eacute;r&eacute;</trd>,
    <trd>outr&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>m&eacute;rt&eacute;ktelen</trd>,
    <trd>szertelen</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>onmatig</trd>,
    <trd>buitensporig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ekstremalny</trd>,
    <trd>przesadny</trd>,
    <trd>nieumiarkowany</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>extrem</trd>,
    <trd>exagerat</trd>,
    <trd>imoderat</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_je;&c_u;&c_m;&c_je;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>bezuzdn&yacute;</trd>,
    <trd>nestriedmy</trd>
  </trdgrp>
</drv>
</art>
