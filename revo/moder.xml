<vortaro>
<art mrk="$Id: moder.xml,v 1.24 2024/01/19 09:24:54 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>moder</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="moder.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="moder.0a.KOMUNE">
    <dif>
      Ne <ref cel="tro.0a">troa</ref>, ne <ref tip="ant" cel="eksces.0a">ekscesa</ref>, ne
      <ref tip="ant" cel="ekstre.0a.kvante">ekstrema<sncref/></ref>:
      <ekz>
        se ili neniam restas sen rimedoj, tio estas la merito de ilia <tld/>a vivo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sa&gcirc;ulo sciis esti <tld/>a: li montris la internan vundon de l' &scirc;tato, sed li
        ne flamigis &gcirc;in
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        li nun sentis al &scirc;i amon nur <tld/>an
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        li sidi&gcirc;is &ccirc;e la kartotablo kun esprimo de <tld/>a plezuro
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>;
      </ekz>
      <ekz>
        vivo <tld/>a estas vivo sendan&gcirc;era
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a ru&gcirc;eco de la konjunktivo
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mez.0a">meza</ref>,
      <ref cel="modest.0a">modesta</ref>,
      <ref cel="sobr.0a">sobra</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_r;&c_y;&c_m;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_ja;&c_r;&c_k;&c_o;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_u;&c_m;&c_je;&c_r;&c_je;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>decentn&iacute;</trd>,
    <trd>m&iacute;rn&yacute;</trd>,
    <trd>um&iacute;rn&ecaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>m&auml;&szlig;ig</trd>,
    <trd>gem&auml;&szlig;igt</trd>
  </trdgrp>
  <trd lng="en">moderate</trd>
  <trd lng="es">moderado, -a</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&f_y;&a_A;&a_n;&a_h;&#8204;&a_r;&a_w;</trd>,
    <trd>&a_m;&a_ayn;&a_t;&a_d;&a_l;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>m&eacute;diocre</trd>,
    <trd>mod&eacute;r&eacute;</trd>,
    <trd>modique</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>m&eacute;rs&eacute;kelt</trd>,
    <trd>m&eacute;rt&eacute;kletes</trd>,
    <trd>m&eacute;rt&eacute;ktart&oacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#36969;&#x5EA6;&#x306E;  <pr>&#12390;&#x304D;&#12393;&#x306E;</pr></trd>,
    <trd>&#x624B;&#12372;&#x308D;&#x306A;  <pr>&#12390;&#12372;&#x308D;&#x306A;</pr></trd>,
    <trd>&#x307B;&#12393;&#x307B;&#12393;&#x306E;</trd>,
    <trd>&#x7BC0;&#x5EA6;&#12354;&#x308B;  <pr>&#x305B;&#12388;&#12393;&#12354;&#x308B;</pr></trd>,
    <trd>&#x7A4F;&#20581;&#x306A;  <pr>&#x304A;&#12435;&#12369;&#12435;&#x306A;</pr></trd>,
    <trd>&#x4E2D;&#x5EB8;&#x306A;  <pr>&#12385;&#12421;&#12358;&#12424;&#12358;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>matig</trd>,
    <trd>gematigd</trd>
  </trdgrp>
  <trdgrp lng="pl">
