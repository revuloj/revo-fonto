<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: distri1.xml,v 1.24 2022/04/26 17:11:48 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>distrikt</rad>/o
</kap>
<drv mrk="distri1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Precize limita <ref tip="super" cel="terito.0o">teritorio</ref>
      havanta apartan administran unuecon:
      <ekz>
        ili venis en la <tld/>ojn &ccirc;e <nom>Jordan</nom>, kiuj estas en la lando Kanaana
        <fnt>
          <bib>MT</bib>
          <lok>Josuo 22:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        apud ili konstruis <nom>Refaja</nom>, filo de <nom>&Hcirc;ur</nom>,
        estro de duon<tld/>o de <nom>Jerusalem</nom>
        <fnt>
          <bib>MT</bib>
          <lok>Ne&hcirc;emja 3:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        li iris <klr>[&#x2026;]</klr> al la <ind><tld/>estro</ind>, kiu raportis pri tio en Kopenhagon
        <fnt>
          <bib>Fab3</bib>
          <lok>Ib kaj malgranda kristino</lok>
        </fnt>;
        <trd lng="de">Bezirksvorsteher</trd>
      </ekz>
      <ekz>
        alveturis oficisto kun la ordono revizi la 
        tutan <ref tip="malprt" cel="gubern1.0o">gubernio</ref>n kaj precipe nian
        <tld/>on
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pastro de mia <tld/>o
        <klr>(<ref tip="sub" cel="parohx.0o">paro&hcirc;o</ref>)</klr>
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la 17a viena <tld/>o
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/006938.html">Arkitekturo: Friedensreich Hundertwasser 1928 - 2000</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        konvencia nomo por indiki la tutan <tld/>on de Tokio kaj
        kelkaj najbaraj gubernioj
        <fnt><bib>EncJap</bib><lok>artikolo &leftquot;kant&ocirc;&rightquot;, p. 87a</lok></fnt>;
      </ekz>
      <ekz>
        komisaro de la administra <tld/>o <klr>[&#x2026;]</klr> Ekvatoro, teritorio 12
        fojojn pli granda ol Belgio
        <fnt><aut>Wim M. A. De Smet</aut>:
        <vrk>Anta&ubreve; cent jaroj</vrk>, <bib>Monato</bib>
        <lok>jaro 1998a, numero 5a, p. 25a</lok></fnt>;
      </ekz>
      <ekz>
        Bolcman-strateto, 9a <tld/>o
        <fnt><aut>Walter Klag</aut>:
        <vrk>Bariloj kontra&ubreve; bomboj</vrk>, <bib>Monato</bib>
        <lok>jaro 1999a, numero 3a, p. 7a</lok></fnt>;
      </ekz>
      <ekz>
        pluraj albanoj sekve opiniis, ke Kosovo estas nur iu <tld/>o 
        a&ubreve; granda vila&gcirc;o, kaj ne vasta lando
        <fnt>
        <aut>Bardhyl Selimi</aut>: <vrk>Voja&gcirc;impresoj, somero 2000
        </vrk>, <bib>Monato</bib><lok>jaro
        2001a, numero 1a, p. 26a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kanton.0o">kantono</ref>,
      <ref cel="kvarta.0o">kvartalo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_jo;&c_n;</trd>,
    <trd>&c_a;&c_k;&c_r;&c_u;&c_g;&c_a;</trd>,
    <trd>&c_u;&c_ch;&c_a;&c_s;&c_t;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">districte</trd>
  <trdgrp lng="cs">
    <trd>distrikt</trd>,
    <trd>kraj</trd>,
    <trd>obvod</trd>,
    <trd>okres</trd>,
    <trd>rajon</trd>,
    <trd>rev&iacute;r</trd>,
    <trd>spr&aacute;vn&iacute; &uacute;zem&iacute;</trd>,
    <trd>&uacute;jezd</trd>,
    <trd>&zcaron;upa</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bezirk</trd>,
    <trd>Distrikt</trd>,
    <trd>Revier</trd>
  </trdgrp>
  <trd lng="en">district</trd>
  <trd lng="es">distrito</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_kh;&a_sh;</trd>,
    <trd>&a_n;&a_A;&a_H;&f_y;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">district</trd>
  <trd lng="hu">ker&uuml;let<klr tip="ind"> (v&aacute;rosnegyed)</klr>
  </trd>
  <trd lng="nl">district</trd>
  <trdgrp lng="pl">
    <trd>obw&oacute;d</trd>,
    <trd>powiat</trd>,
    <trd>dystrykt</trd>,
    <trd>okr&eogonek;g</trd>,
    <trd>hufiec<klr> (w harcerstwie)</klr></trd>
  </trdgrp>
  <trd lng="pt">distrito</trd>
  <trd lng="ro">circumferin&tcedil;&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_k;&c_r;&c_u;&c_g;</trd>,
    <trd>&c_u;&c_ch;&c_a;&c_s;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_r;&c_a;&c_j;&c_o;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>okres</trd>,
    <trd>raj&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x43A;&#x440;&#x443;&#x433;</trd>,
    <trd>&#x440;&#x430;&#x439;&#x43E;&#x43D;</trd>,
    <trd>&#x43F;&#x43E;&#x432;&#x456;&#x442;</trd>,
    <trd>&#x434;&#x438;&#x441;&#x442;&#x440;&#x438;&#x43A;&#x442;</trd>
  </trdgrp>
</drv>

<drv mrk="distri1.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a lernejo
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>a ju&gcirc;ejo</ind>
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto unua</lok>
        </fnt>;
        <trd lng="de">Bezirksgericht</trd>
      </ekz>
      <ekz>
        nia <tld/>a nobelestro
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto unua</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x43A;&#x440;&#x443;&#x436;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x440;&#x430;&#x439;&#x43E;&#x43D;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x43F;&#x43E;&#x432;&#x456;&#x442;&#x43E;&#x432;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="distri1.sub0o">
  <kap>
    sub<tld/>o
  </kap>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Subdivido de <ref tip="malprt" cel="distri1.0o"><tld/>o</ref>:
      <ekz>
        la kuracloko <nom>Zell am See</nom> estas la &ccirc;efa respektive administra urbo de la samnoma
        sub<tld/>o <klr>[&#x2026;]</klr> en la a&ubreve;stria federacia lando Salcburg<klr>[o]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=294810">Zell am See</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Kreis</trd>
  <trd lng="fr">arrondissement</trd>
</drv>





</art>
