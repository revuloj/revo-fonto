<vortaro>
<art mrk="$Id: virgini.xml,v 1.9 2024/06/26 23:27:45 revo Exp $">
<kap>
  <rad>Virgini</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="virgini.0o">
  <kap><tld/>o</kap>
  <snc mrk="virgini.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Sub&scirc;tato en la orienta parto de
      <ref tip="malprt" cel="uson.U0o.GEOG">Usono</ref>:
      <ekz>
        psikologo en <tld/>o diras, ke oni neniam devus mensogi, krom
        eble en pokero kaj golfludado, liaj solaj esceptoj
        <fnt>
          <aut>A. J. Jacobs , trad. J. Piton</aut>,
          <vrk><url
          ref="https://www.ted.com/talks/a_j_jacobs_year_of_living_biblically/transcript?language=eo">
          La jaro de la&ubreve;biblia vivado</url></vrk>,
          <lok>Ted, 2008-07</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_V;&c_ib;&c_r;&c_d;&c_zh;&c_y;&c_n;&c_ib;&c_ja; <klr>(&c_sh;&c_t;&c_a;&c_t; &c_Z;&c_Sh;&c_A;)</klr></trd>
  <trd lng="cs">Virg&iacute;nie</trd>
  <trd lng="en">Virginia</trd>
  <trd lng="fr">Virginie</trd>
  <trd lng="ja">&#x30D0;&#x30FC;&#x30B8;&#x30CB;&#x30A2;&#x5DDE; 
  <pr>&#x30D0;&#x30FC;&#x30B8;&#x30CB;&#x30A2;&#12375;&#12421;&#12358;</pr></trd>
  <trd lng="pl">Wirginia</trd>
  <trd lng="sk">Virg&iacute;nia</trd>
  <trd lng="uk">&c_V;&c_ib;&c_r;&c_d;&c_zh;&c_i;&#x43D;&c_ib;&#x44F; (&c_sh;&c_t;&c_a;&c_t;)</trd>
  <trdgrp lng="zh">
    <trd>&#x4F5B;&#21513;&#x5C3C;&#x4E9A; <pr>F&#xFA;j&#xED;n&#xED;y&#xE0;</pr></trd>,
    <trd>&#x4F5B;&#21513;&#x5C3C;&#x4E9E; <pr>F&#xFA;j&#xED;n&#xED;y&#xE0;</pr></trd>,
    <trd>&#x5F17;&#21513;&#x5C3C;&#x4E9A;&#x5DDE; <pr>F&#xFA;j&#xED;n&#xED;y&#xE0;zh&#x14D;u</pr></trd>,
    <trd>&#x5F17;&#21513;&#x5C3C;&#x4E9E;&#x5DDE; <pr>F&#xFA;j&#xED;n&#xED;y&#xE0;zh&#x14D;u</pr></trd>,
    <trd>&#x5F17;&#x5DDE; <pr>F&#xFA;zh&#x14D;u</pr></trd>
  </trdgrp>
</drv>

<drv mrk="virgini.okcidenta0o">
  <kap>Okcidenta <tld/>o,
  <var><kap>Okcident<tld lit="v"/>o</kap></var></kap>
  <snc mrk="virgini.okcidenta0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Sub&scirc;tato en la orienta parto de
      <ref tip="malprt" cel="uson.U0o.GEOG">Usono</ref>:
      <ekz>
        la&ubreve; sveda ekonomikisto <klr>...</klr> Svedio
        pli malri&ccirc;as ol &ccirc;iuj usonaj sub&scirc;tatoj krom Okcident-<tld/>o
        kaj Misisipo
        <fnt>
          <aut>K. Miner</aut>,
          <vrk><url ref="http://www.sunflower.com/~miner/TAGLIBRO_package/Januaro_2005.html">
          taglibro</url></vrk>,
          <lok>2005-01-14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_Z;&c_a;&c_h;&c_o;&c_d;&c_n;&c_ja;&c_ja; &c_V;&c_ib;&c_r;&c_d;&c_zh;&c_y;&c_n;&c_ib;&c_ja; <klr>(&c_sh;&c_t;&c_a;&c_t; &c_Z;&c_Sh;&c_A;)</klr></trd>
  <trd lng="en">West Virginia</trd>
  <trd lng="fr">Virginie occidentale</trd>
  <trd lng="pl">Wirginia Zachodnia</trd>
  <trdgrp lng="zh">
    <trd>&#x897F;&#x5F17;&#21513;&#x5C3C;&#x4E9A; <pr>X&#x12B;F&#xFA;j&#xED;n&#xED;y&#xE0;</pr></trd>,
    <trd>&#x897F;&#x5F17;&#21513;&#x5C3C;&#x4E9E; <pr>X&#x12B;F&#xFA;j&#xED;n&#xED;y&#xE0;</pr></trd>,
    <trd>&#x897F;&#x5F17;&#21513;&#x5C3C;&#x4E9A;&#x5DDE;
    <pr>X&#x12B;F&#xFA;j&#xED;n&#xED;y&#xE0;zh&#x14D;u</pr></trd>,
    <trd>&#x897F;&#x5F17;&#21513;&#x5C3C;&#x4E9E;&#x5DDE;
    <pr>X&#x12B;F&#xFA;j&#xED;n&#xED;y&#xE0;zh&#x14D;u</pr></trd>
  </trdgrp>
</drv>
</art>

