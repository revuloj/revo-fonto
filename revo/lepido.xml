<art mrk="$Id: lepido.xml,v 1.21 2023/09/01 19:10:59 revo Exp $">
<kap>
  <rad>lepidodendr</rad>/o
</kap>
<drv mrk="lepido.0o">
  <kap><tld/>o</kap>
  <snc mrk="lepido.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="fak">PAL</uzo>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      de fosiliaj arboj, el <ref tip="malprt" cel="likopo.0oplantoj">likopodioplantoj</ref>,
      &gcirc;is 30&#x202F;metrojn altaj (<trd lng="la">Lepidodendron</trd>):
      <ekz>
        la <tld/>ojn, oftaj dum karbonio, oni rekonas per skvamaj
        fosilioj, postsignoj de la folicikatroj
        sur la trunko;
      </ekz>
      <ekz>
        <tld/>oj estas proksimaj parencoj de izoetoj.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/4/41/Tronco_y_ra&iacute;z_de_Lepidodendron.jpg/640px-Tronco_y_ra&iacute;z_de_Lepidodendron.jpg" prm="CC-BY-SA-3.0">fosilioj de trunko kaj radiko de <tld/>o (Lepidodendron
      aculeatun) el Muzeo de
        Cordoba, Hispanujo
        <fnt>
          <aut>Lepidodendron_aculeatum2.jpg:  Pablo Alberto Salguero
          Quilesderivative work: CASF (talk),
          Lepidodendron_aculeatum2.jpg</aut>
          <lok><url ref="&WCW;/File:Tronco_y_ra%C3%ADz_de_Lepidodendron.jpg">&WCW;/File:Tronco_y_ra&iacute;z_de_Lepidodendron.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_l;&c_je;&c_p;&c_ib;&c_d;&c_a;&c_d;&c_e;&c_n;&c_d;&c_r;&c_a;&c_n;</trd>
  <trd lng="ca">g&egrave;nere Lepidodendron</trd>
  <trd lng="cs">p&rcaron;esli&ccaron;ka</trd>
  <trd lng="de">Schuppenbaum</trd>
  <trd lng="fr">lepidodendron</trd>
  <trd lng="hu">pikkelyfa</trd>
  <trd lng="ja">&#x30EA;&#x30F3;&#x30DC;&#x30AF;</trd>
  <trdgrp lng="nl">
    <trd>schubboom</trd>,
    <trd>lepidodendron</trd>
  </trdgrp>
  <trd lng="pl">lepidodendron</trd>
  <trd lng="pt">lepidodendro</trd>
  <trd lng="ro">lepidodendron</trd>
  <trd lng="ru">&c_l;&c_je;&c_p;&c_i;&c_d;&c_o;&c_d;&c_je;&c_n;&c_d;&c_r;&c_o;&c_n;
  </trd>
  <trd lng="sk">prasli&ccaron;ka</trd>
  <trd lng="uk">&#x43B;&c_je;&#x43F;&c_ib;&c_d;&#x43E;&c_d;&c_je;&#x43D;&c_d;&c_r;&#x43E;&#x43D;</trd>
</drv>
</art>
