<art mrk="$Id$">
<kap>
  <rad>lepidodendr</rad>/o
</kap>
<drv mrk="lepido.0o">
  <kap><tld/>o</kap>
  <snc mrk="lepido.0o.BOT">
    <uzo tip="fak">BOT</uzo>
