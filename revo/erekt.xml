  <kap><rad>erekt</rad>/a</kap>

<drv mrk="erekt.0a">
  <kap><tld/>a</kap>
  <snc>
  <uzo tip="fak">FIZL</uzo>
    <dif>
      Malmola kaj &scirc;velinta pro sangalfluo (precipe pri peniso a&ubreve;
      klitoro):
      <ekz>
        la pentristo prezentis grandegajn virajn generilojn
        (duon-<tld/>an penison kaj testikojn)
        <fnt>
          <aut>K. A. Efetov</aut>:
          <vrk>La &scirc;oka mistero de la Siksta kapelo</vrk>.
          <lok>&leftquot;Kontakto&rightquot;, n-ro 228 (2008:06). P. 9a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_r;&c_e;&c_g;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
  <trd lng="ca">erecte</trd>
  <trd lng="cs">ztopo&rcaron;en&yacute;</trd>
  <trd lng="en">erect</trd>
  <trd lng="fr">&eacute;rectile</trd>
  <trd lng="ja">&#21187;&#36215;&#12375;&#12383;  <pr>&#12412;&#12387;&#12365;&#12375;&#12383;</pr></trd>
  <trd lng="pl">wyprostowany</trd>
  <trd lng="ru">&c_e;&c_r;&c_je;&c_g;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">stoporen&yacute;</trd>
  <trd lng="uk">&c_je;&c_r;&c_je;&c_g;&c_o;&c_v;&c_a;&c_n;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#21082; <pr>z&igrave;</pr></trd>,
    <trd>&#21187;&#36215; <pr>b&oacute;q&#464;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="erekt.0o">
  <kap><tld/>o, <var><kap><tld/>i&gcirc;o</kap>
    </var>
  </kap>
  <snc>
  <uzo tip="fak">FIZL</uzo>
    <dif>
      La stato de organo kiu i&gcirc;as a&ubreve; estas <tld/>a:
      <ekz>
        estis nenia korpa indiko, nature; nenia <tld/>o a&ubreve; io tia
        <fnt>
          <aut>John WAIN, tr. Marjorie BOULTON</aut>:
          <vrk>Sinjoreto Rikardo</vrk>.
          <lok><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr73/rikardo.html">La nica
          literatura revuo 7:3 (n-ro 39)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_r;&c_e;&c_k;&c_c;&c_y;&c_ja;</trd>
  <trd lng="ca">erecci&oacute;</trd>
  <trd lng="en">erection</trd>
  <trd lng="fr">&eacute;rection</trd>
  <trd lng="he">&zayin;&#1460;&qof;&pe;&#1468;&#1464;&he;</trd>
  <trd lng="ja">&#21187;&#36215;  <pr>&#12412;&#12387;&#12365;</pr></trd>
  <trd lng="pl">erekcja</trd>
  <trd lng="ro">erec&#539;ie</trd>
  <trd lng="ru">&c_e;&c_r;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>
  <trd lng="uk">&c_je;&c_r;&c_je;&c_k;&c_c;&c_ib;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#31435;&#36215; <pr>l&igrave;q&#464;</pr></trd>,
    <trd>&#21187;&#36215; <pr>b&oacute;q&#464;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="erekt.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="erekt.0igxi.FIZL">
  <uzo tip="fak">FIZL</uzo>
    <dif>
      I&gcirc;i <tld/>a:
      <ekz>
        la kresto de koko <tld/>i&gcirc;as <fnt><bib>PIV2</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_r;&c_e;&c_g;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">posar-se en erecci&oacute;</trd>
  <trd lng="fr">entrer en <ind>&eacute;rection</ind></trd>
  <trd lng="he">&lamed;&he;&zayin;&dalet;&qof;&fpe;</trd>
  <trd lng="ja">&#21187;&#36215;&#12377;&#12427;  <pr>&#12412;&#12387;&#12365;&#12377;&#12427;</pr></trd>
  <trd lng="pl">prostowa&cacute; si&eogonek;</trd>
  <trd lng="ru">&c_e;&c_r;&c_je;&c_g;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="uk">
    <trd>&c_je;&c_r;&c_je;&c_g;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_p;&c_r;&c_ja;&c_m;&c_l;&c_ja;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

</art>

