<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: polihxet.xml,v 1.6 2019/02/17 17:10:23 revo Exp $">
<kap>
  <rad>poli&hcirc;et</rad>/oj <fnt>PIV2</fnt>
</kap>

<drv mrk="polihxet.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Klaso de <ref tip="super" cel="verm.ringo0oj">ringovermoj</ref>,
      2&#8239;mm &ndash; 3&#8239;m longaj, kun plursegmenta korpo kaj primitivaj
      membroj kun multaj haregoj, kun pli ol 7&#8239;mil specioj, vivantaj precipe en
      la maroj &ccirc;e la fundo:
      <ekz>
        <tld/>oj estas klaso de anelidoj, unu el la plej sukcesaj kaj praaj animalaj
        grupoj, la <tld/>oj ricevis sian nomon pro la multnombraj &hcirc;etoj sur &ccirc;iu
        segmento kaj kiel apogaj kaj anta&ubreve;movaj aparatoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=81088"
              >Poli&hcirc;etoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="oligohx.0oj">oligo&hcirc;etoj</ref>
    <bld
      lok="&WCU;/thumb/8/89/Borstenwurmer_des_Meeres.png/327px-Borstenwurmer_des_Meeres.png"
      prm="CC BY-SA 3.0"
      >diversaj <tld/>oj el Matthias Jacob Schleiden: &leftquot;Das Meer&rightquot; p. 446
        <fnt>
          <aut>prilaborita de Martin (talk)</aut>
          <lok><url ref="&WCW;/File:Borstenwurmer_des_Meeres.png"
              >https://commons.wikimedia.org/wiki/File:Borstenwurmer_des_Meeres.png</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="bg">&c_m;&c_n;&c_o;&c_g;&c_o;&c_ch;&c_je;&c_t;&c_i;&c_n;&c_je;&c_s;&c_t;&c_i; &c_ch;&c_je;&c_r;&c_v;&c_je;&c_i;</trd>
  <trd lng="de">Vielborster</trd>
  <trd lng="en">polychaete</trd>
  <trd lng="fr">polych&egrave;tes</trd>
  <trd lng="hu">soks&ouml;rt&eacute;j&udblac;ek</trd>
  <trd lng="nl">borstelwormen</trd>
  <trd lng="la">polychaeta</trd>
  <trd lng="pt">poliquetas</trd>
  <trd lng="ru">&c_m;&c_n;&c_o;&c_g;&c_o;&c_shch;&c_je;&c_t;&c_i;&c_n;&c_k;&c_o;&c_v;&c_y;&c_je;</trd>
</drv>
</art>


