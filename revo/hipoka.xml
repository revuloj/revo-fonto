<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>hipoka&scirc;tan</rad>/o
</kap>
<drv mrk="hipoka.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="hipoka.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Aesculus</trd>),
      kies neman&gcirc;ebla frukto similas ka&scirc;tanon.
    </dif>
    <ref tip="vid" cel="kasxta.0arbo0ujo">ka&scirc;tanujo</ref>
  </snc>
  <bld lok="../bld/hipoka.jpg">floraro kaj folioj,
  (<trd lng="la">Aesculus hippocastanum</trd>)</bld>
  <trdgrp lng="de">
    <trd>Ro&szlig;kastanie</trd>, <trd>Kastanie</trd>
  </trdgrp>
  <trd lng="en">horse chestnut</trd>
</drv>
<drv mrk="hipoka.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      arboj kaj arbedoj (<trd lng="la">Hippocastanaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Ro&szlig;kastaniengew&auml;chse</trd>
</drv>
</art>
