<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bori.xml,v 1.20 2023/12/11 18:33:39 revo Exp $">
<kap>
  <rad>bori</rad>/o
</kap>
<drv mrk="bori.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Bh</mlg>
  <snc>
  <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="107">elemento</ref>
      kun
      atomnumero 107, atompezo 262,1229, nomita la&ubreve; la dana
      fizikisto <nom>Niels Bohr</nom>; <ref tip="super" cel="urani.trans0o">transuranio</ref>, kies
      &ccirc;iuj <ref tip="super" cel="izotop.0o">izotopoj</ref>
      estas radioaktivaj:
      <ekz>
        transirmetaloj estas: <klr>[&#8230;]</klr> ruterfordio, dubnio, seborgio, <tld/>o, hasio
        <klr>[&#8230;]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4279">Transirmetalo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Niels Bohr</nom> <klr>[&#8230;]</klr> <tld/>o, la kemia elemento, nomi&gcirc;as tiel en lia honoro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17316">Niels Bohr</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      <nom>IUPAC</nom> en 1997 jaro findecidis pri la nomo borio,
      anta&ubreve;e estis uzata provizore la nomo unnilseptio
      <klr>(science <nac>Unnilseptium</nac>)</klr>.
    </rim>
  </snc>
  <trd lng="be">&c_b;&c_o;&c_r;&c_y;&c_j;</trd>
  <trd lng="bg">&c_b;&c_o;&c_r;&c_i;&c_j;</trd>
  <trd lng="cs">b&oacute;r</trd>
  <trd lng="de">Bohrium</trd>
  <trd lng="fa">&a_b;&a_w;&a_r;&f_y;&a_m;</trd>
  <trd lng="fr">bohrium</trd>
  <trd lng="hu">bohrium</trd>
  <trd lng="id">bohrium</trd>
  <trd lng="la">Bohrium</trd>
  <trd lng="nl">bohrium</trd>
  <trd lng="pl">bohr</trd>
  <trd lng="ru">&c_b;&c_o;&c_r;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>b&oacute;rium</trd>,
    <trd>b&oacute;rium</trd>
  </trdgrp>
  <trd lng="sv">bohrium</trd>
  <trd lng="uk">&c_b;&#x43E;&c_r;&c_ib;&c_j;</trd>
</drv>
</art>
