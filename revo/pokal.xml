<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pokal.xml,v 1.31 2024/02/17 07:52:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pokal</rad>/o
</kap>
<drv mrk="pokal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pokal.0o.vazo">
    <dif>
      Iom luksa, funelforma <ref tip="super" cel="trink.0ujo">trinkujo</ref>,
      kun kruro kaj piedo, ofte kun kovrilo:
    </dif>
  <subsnc mrk="pokal.0o.trinkujo">
    <dif>
      Tia ujo destinita por trinkado de
      vino, biero, alkohola a&ubreve; iel valora trinka&jcirc;o:
      <ekz>
        inter <tld/>o kaj lipoj povas multe okazi
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        altaj kru&ccirc;oj por vino, skatoletoj kun boteletoj de parfumoj,
        oraj kaj ar&gcirc;entaj <tld/>oj kaj glasetoj, fajencaj supujoj
        kaj pladoj
        <fnt><bib>Far1</bib> <lok>&ccirc;ap. 9</lok></fnt>;
      </ekz>
      <ekz>
        <nom>Sargon</nom> ordonis alporti al si grandan vitran <tld/>on
        <fnt><bib>Far2</bib> <lok>&ccirc;ap. 10</lok></fnt>;
      </ekz>
      <ekz>
        salono plenigita per muziko kanto rido kaj sonoj de <tld/>oj
        <fnt><bib>Far2</bib> <lok>&ccirc;ap. 6</lok></fnt>;
      </ekz>
      <ekz>
        li man&gcirc;is tritikan platan kukon, plenmanon da daktiloj
        kaj trinkis <tld/>on da malforta biero
        <fnt><bib>Far1</bib> <lok>&ccirc;ap. 5</lok></fnt>;
      </ekz>
      <ekz>
        li profunde ekspiris kaj eltrinkinte grandan <tld/>on
        da vino komencis mediti
        <fnt><bib>Far1</bib> <lok>&ccirc;ap. 14</lok></fnt>;
      </ekz>
      <ekz>
        mi fari&gcirc;u Hebreo se unu pokalo da tia nektaro ne
        valoras du dra&hcirc;mojn
        <fnt><bib>Far1</bib> <lok>&ccirc;ap. 19</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kalik.0o">kaliko</ref>
    <trdgrp lng="be">
      <trd>&c_k;&c_je;&c_l;&c_ib;&c_h;</trd>,
      <trd>&c_k;&c_ib;&c_l;&c_ib;&c_sh;&c_a;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>cup</trd>,
      <trd>goblet</trd>
    </trdgrp>
    <trd lng="nl">bokaal</trd>
    <trd lng="pl">puchar  <klr>(naczynie)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_b;&c_o;&c_k;&c_a;&c_l;</trd>,
      <trd>&c_r;&c_ju;&c_m;&c_k;&c_a;</trd>
    </trdgrp>
  </subsnc>
  <subsnc mrk="pokal.0o.trofeo">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Tia ujo el valora metalo donata kiel
      <ref tip="super" cel="trofe.0o">trofeo</ref>
      en konkursoj:
      <ekz>
        Oratora Konkurso siatempe premiita per
        la ar&gcirc;enta &leftquot;<tld lit="P"/>o Ivo <nom>Lapenna</nom>"
        <fnt><bib>MemLapenna</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_u;&c_b;&c_a;&c_k;
      <klr>(&c_s;&c_p;&c_a;&c_r;&c_t;&c_o;&c_v;&c_y;)</klr></trd>
    <trdgrp lng="en">
      <trd>cup</trd>,
      <trd>trophy</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>beker<klr tip="ind"> (trofee)</klr></trd>,
      <trd>trofee</trd>
    </trdgrp>
    <trd lng="pl">puchar</trd>
    <trd lng="ru">&c_k;&c_u;&c_b;&c_o;&c_k;</trd>
  </subsnc>
  </snc>
  <snc mrk="pokal.0o.SPO">
    <uzo tip="klr">(metonimie)</uzo>
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref cel="konkur2.0o.SPO">Konkurso<sncref/></ref>
      en kiu la venkinto ricevas <ref cel="pokal.0o.trofeo"><tld/>on<sncref/></ref>:
      <ekz>
        &ccirc;irka&ubreve; 300000 turistoj vizitis la plej okcidentan regionon de Ruslando; la
        direktoro de la loka flughaveno sciigis, ke &ccirc;iusemajna kvanto da aviaj
        pasa&gcirc;eroj superis tiun de la periodo de la mond<tld/>o en 2018
        <fnt>
          <aut>H. Gorecka</aut>
          <vrk><url ref="http://radio.esperanto-ondo.ru/Arkivoj/podkasto/55">Okcidente de Ruslando:
          Kaliningrada regiono turisme</url></vrk>
          <lok>Radio Esperanto, 2020-10-18 (55)</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_u;&c_b;&c_a;&c_k; <klr>(&c_s;&c_p;&c_a;&c_b;&c_o;&c_r;&c_n;&c_ib;&c_c;&c_t;&c_v;&c_a;)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>kalich</trd>,
    <trd>poh&aacute;r</trd>,
    <trd>pok&aacute;l</trd>,
    <trd>&ccaron;&iacute;&scaron;e</trd>
  </trdgrp>
  <trd lng="de">Pokal</trd>
  <trd lng="fr">coupe</trd>
  <trd lng="pt">ta&ccedil;a</trd>
  <trd lng="ro">cea&#x219;c&abreve;</trd>
  <trdgrp lng="sk">
    <trd>kalich</trd>,
    <trd>poh&aacute;r</trd>,
    <trd>&ccaron;a&scaron;a</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5956;&#x676F; <pr>ji&#x1CE;ngb&#x113;i</pr></trd>,
    <trd>&#x734E;&#x676F; <pr>ji&#x1CE;ngb&#x113;i</pr></trd>,
    <trd>&#x676F; <pr>b&#x113;i</pr></trd>,
    <trd>&#x76C3; <pr>b&#x113;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pokal.P0o">
  <kap><tld lit="P"/>o
    <fnt>
      <aut>D. Galad&iacute;-Enr&iacute;quez, A. Wandel</aut>
      <vrk>La kosmo kaj ni</vrk>
      <lok>2005</lok>
    </fnt>
  </kap>
  <mlg>Crt</mlg>
  <snc mrk="pokal.P0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Sud-hemisfera <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref>
      (<trd lng="la">Crater</trd>).
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_K;&c_je;&c_l;&c_ib;&c_h; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#x2019;&c_je;)</klr></trd>,
    <trd>&c_Ch;&c_a;&c_sh;&c_a; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#x2019;&c_je;)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kalich</trd>,
    <trd>poh&aacute;r</trd>,
    <trd>pok&aacute;l</trd>,
    <trd>&ccaron;&iacute;&scaron;e</trd>
  </trdgrp>
  <trd lng="de">Becher</trd>
  <trd lng="en">Crater</trd>
  <trd lng="es">Copa</trd>
  <trd lng="fr">Coupe<klr tip="ind"> (constellation)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x676F;  <pr>&#x306F;&#x305F;</pr></trd>,
    <trd>&#x30B7;&#x30E3;&#x30F3;&#x30DA;&#x30F3;&#x30B0;&#x30E9;&#x30B9;</trd>,
    <trd>&#x30C8;&#x30ED;&#x30D5;&#x30A3;&#x30FC;&#x30AB;&#x30C3;&#x30D7;</trd>,
    <trd>&#x512A;&#x52DD;&#x676F;  <pr>&#x3086;&#x3046;&#x3057;&#x3087;&#x3046;&#x306F;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">Beker<klr tip="ind"> (sterrenbeeld)</klr></trd>
  <trd lng="pl">Puchar  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Cupa  <klr>(constela&#x21B;ie)</klr></trd>
  <trdgrp lng="sk">
    <trd>kalich</trd>,
    <trd>poh&aacute;r</trd>,
    <trd>&ccaron;a&scaron;a</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_je;&#x43B;&c_i;&c_h;</trd>,
    <trd>&#x43A;&c_u;&c_b;&#x43E;&#x43A;</trd>
  </trdgrp>
</drv>

</art>
