<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc> <ofc>8</ofc>
  <rad>atlas</rad>/o
</kap>
<drv mrk="atlas.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Dol&ccirc;emola, brila kaj glata speco de silka&jcirc;o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a ha&ubreve;to.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Satin</trd>,
      <trd>Atlasseide</trd>
    </trdgrp>
    <trd lng="en">satin</trd>
    <trdgrp lng="fr">
      <trd>satin</trd>,
      <trd>atlas (tissu)</trd>
    </trdgrp>
    <trd lng="nl">satijn</trd>
    <trdgrp lng="sv">
      <trd>sat&auml;ng</trd>,
      <trd>satin</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">EVI</uzo>
    <dif>
      Kolekto da geografiaj kartoj, kaj pli &gcirc;enerale da desegna&jcirc;oj,
      figuroj por speciala fako:
      <ekz>
        <tld/>o geografia, historia, anatomia, moda.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Atlas</trd>,
      <trd>Kartenwerk</trd>
    </trdgrp>
    <trd lng="en">atlas</trd>
    <trd lng="nl">atlas</trd>
    <trd lng="fr">atlas<klr tip="ind"> (g&eacute;ographique)</klr></trd>
    <trd lng="sv">atlas</trd>
  </snc>
  <trd lng="ru">&c_a;&c_t;&c_l;&c_a;&c_s;</trd>
</drv>
</art>
