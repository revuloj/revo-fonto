<drv mrk="vent.0a">
  <kap><tld/>a</kap>
  <snc mrk="vent.0a.MET">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a vetero
        <fnt><bib>PatrojFiloj</bib>, <lok>&Ccirc;ap. 22a.</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a energio
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="https://www.monato.be/2005/008577.php">Venta
          energio superas la atoman</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a, sen&scirc;irma, senarba <klr>[&#x2026;]</klr> insuleto
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="https://www.monato.be/2007/009255.php">Insulo
          de verkistoj</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="id">ber<ind>angin</ind></trd>
    <trdgrp lng="it">
      <trd>ventoso</trd>,
      <trd>ventilato</trd>,
      <trd>eolico</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>wietrzny</trd>,
      <trd>wiatrowy</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_je;&c_t;&c_r;&c_o;&c_v;&c_o;&c_j;</trd>,
      <trd>&c_v;&c_je;&c_t;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="vent.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senbaza, senvalora, malfirma, aera:
      <ekz>
        &ccirc;io estas vanta&jcirc;o kaj entreprenoj <tld/>aj
        <fnt>
          <bib>MT</bib>
          <lok>Predikanto 1:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;io estas vanta&jcirc;o kaj <tld/>a&jcirc;o, kaj nenia profito
        <fnt>
          <bib>MT</bib>
          <lok>Predikanto 2:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u sa&gcirc;ulo povas respondi per <tld/>aj opinioj?
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 15:2</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fum.0o.FIG">fumo <sncref/></ref>
    </refgrp>
    <trd lng="ru">&c_v;&c_je;&c_t;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_v;&c_je;&c_t;&c_r;&c_o;&c_v;&c_i;&c_t;</trd>,
    <trd>&c_v;&c_ja;&c_t;&c_malmol;&c_r;&c_n;&c_i;&c_ch;&c_a;&c_v;</trd>
  </trdgrp>
  <trd lng="cs">v&ecaron;trn&yacute;</trd>
  <trd lng="de">windig</trd>
  <trd lng="fr">fumeux</trd>
  <trdgrp lng="it">
    <trd>fumo<klr> (fig.)</klr></trd>,
    <trd>vento<klr> (fig.)</klr></trd>,
    <trd>aria<klr> (fig.)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vetern&yacute;</trd>,
    <trd>vetrov&yacute;</trd>,
    <trd>vzdu&scaron;n&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="vent.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="vent.0i.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Blovi <klr>(pp <tld/>o)</klr>:
      <ekz>
        forte <tld/>as<fnt>Z</fnt>;
      </ekz>
      <ekz>
        el tombo de prakatakombo plor<tld/>as kantik'
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="http://steloj.de/esperanto/butiko">La Stranga
          Butiko</url></vrk>
          <lok>A&ubreve;tunovesper'</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_mol;&c_m;&c_u;&c_c;&c_mol; 
    <klr tip="amb">(&c_p;&c_r;&c_a; &c_v;&c_je;&c_c;&c_je;&c_r;)</klr></trd>
  <trdgrp lng="cs">
    <trd>b&yacute;t v&ecaron;trno</trd>,
    <trd>foukat</trd>,
    <trd>vanout</trd>
  </trdgrp>
  <trd lng="de">wehen <klr>(Wind)</klr></trd>
  <trd lng="fr">venter</trd>
  <trd lng="hu">f&uacute;j a <ind>sz&eacute;l</ind></trd>
  <trd lng="it">soffiare<klr> (di vento)</klr></trd>
  <trd lng="nl">waaien</trd>
  <trd lng="pl">wia&cacute; <klr>(o wietrze)</klr></trd>
  <trd lng="pt">ventar</trd>
  <trd lng="ru">&c_d;&c_u;&c_t;&c_mol; <klr tip="amb">(&c_b;&c_je;&c_z;&c_l;. &c_o; 
&c_v;&c_je;&c_t;&c_r;&c_je;)</klr></trd>
  <trdgrp lng="sk">
    <trd>f&uacute;ka&tcaron;</trd>,
    <trd>via&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="vent.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="vent.0ego.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Fortega <tld/>o, ordinare akompanata de pluvo a&ubreve; ne&gcirc;o:
      <ekz>
        en tiu nokto blovis terura <tld/>ego kaj la pluvo faladis per
        riveroj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiel pasanta <tld/>ego <klr>[la]</klr> malpiulo malaperas
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 10:25</lok>
        </fnt>;
      </ekz><ekz>
        kvietigi la <tld/>egon
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 107:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        malfeli&ccirc;o venos kiel <tld/>ego
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 1:27</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tempes.0o">tempesto</ref>,
      <ref cel="uragan.0o">uragano</ref>
    </refgrp>
    <trdgrp lng="pl">
      <trd>wicher</trd>,
      <trd>wichura</trd>
    </trdgrp>
    <trd lng="id">badai</trd>
  </snc>
  <snc mrk="vent.0ego.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Rapide trapasanta amaso:
      <ekz>
        la <tld/>egoj de la vivo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la mevoj flirtis &ccirc;irka&ubreve; li kvaza&ubreve; <tld/>ego da
        blankaj flugiloj;
      </ekz><ekz>
        li devis elporti <tld/>egon da ripro&ccirc;oj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lavang.0o">lavango</ref>,
      <ref cel="torent.0o">torento</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_u;&c_r;&c_a;&c_g;&c_a;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>vichr</trd>,
    <trd>vich&rcaron;ice</trd>
  </trdgrp>
  <trd lng="de">Sturm</trd>
  <trd lng="el">&alfa;&ny;&epsilon;&my;&omikron;&zeta;&alfa_ton;&lambda;&eta;</trd>
  <trd lng="fr">temp&ecirc;te</trd>
  <trdgrp lng="hu">
    <trd>vihar</trd>,
    <trd>sz&eacute;lvihar</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>tempesta</trd>,
    <trd>tormenta</trd>
  </trdgrp>
  <trd lng="nl">storm</trd>
  <trd lng="pt">ventania</trd>
  <trd lng="ru">&c_u;&c_r;&c_a;&c_g;&c_a;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>v&iacute;chor</trd>,
    <trd>v&iacute;chrica</trd>
  </trdgrp>
</drv>

<drv mrk="vent.0umi">
  <kap><ofc>*</ofc><tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vent.0umi.aerumi">
    <dif>
      Refre&scirc;igi movante la aeron:
      <ekz>
        li <tld/>umas al si per sia &ccirc;apelo;
      </ekz>
      <ekz>
        &ccirc;iuj damoj <klr>[&#x2026;]</klr> sidis en la unua vico kaj <tld/>umadis
        sin
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tistino kaj kamentubisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        du nigraj sklavoj <tld/>umis lin
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXV</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de"><ind>Luft</ind> zuf&auml;cheln</trd>
    <trd lng="hu">legyez</trd>
    <trd lng="id"><baz>kipas</baz> mengipasi</trd>
    <trd lng="pl">wietrzy&cacute;</trd>
  </snc>
  <snc mrk="vent.0umi.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      <ref cel="ventol.0i.AGR">Ventoli<sncref/></ref>:
      <ekz>
        <nom>Boaz</nom> <klr>[&#x2026;]</klr> <tld/>umas hordeon <klr>[&#x2026;]</klr>
        en la dra&scirc;ejo
        <fnt>
          <bib>MT</bib>
          <lok>Rut 3:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        disfluganta <ind>gren<tld/>uma&jcirc;o</ind>
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 29:5</lok>
        </fnt>
        <klr>(neta&ubreve;ga <tld/>umita parto de la greno)</klr>.
        <trd lng="de">Spreu</trd>
        <trd lng="it">pula</trd>
      </ekz>
    </dif>
    <ref tip="vid" cel="drasx.0i.AGR">dra&scirc;i<sncref/></ref>
    <trdgrp lng="de">
      <trd>windsichten</trd>,
      <trd>die <ind>Spreu</ind> vom Weizen trennen</trd>,
      <trd>worfeln</trd>
    </trdgrp>
    <trd lng="hu">szell&odblac;ztet</trd>
    <trd lng="pl">wia&cacute; <klr>(zbo&zdot;e)</klr></trd>
   </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_v;&c_je;&c_t;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_e;&c_n;&c_t;&c_y;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_v;&c_je;&c_j;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ov&iacute;vat</trd>,
    <trd>profukovat</trd>,
    <trd>prov&ecaron;trat</trd>,
    <trd>vyfukovat</trd>,
    <trd>v&ecaron;trat</trd>,
    <trd>&ccaron;istit</trd>
  </trdgrp>
  <trd lng="el">&alfa;&epsilon;&rho;&jota_ton;&zeta;&omega;</trd>
  <trd lng="fr">ventiler</trd>
  <trdgrp lng="it">
    <trd>ventilare</trd>,
    <trd>sventagliare</trd>,
    <trd>pulare</trd>,
    <trd>dare <ind>aria</ind></trd>
  </trdgrp>
  <trd lng="pt">ventilar</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_v;&c_je;&c_t;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_v;&c_je;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ovia&tcaron;</trd>,
    <trd>prefukova&tcaron;</trd>,
    <trd>prevetra&tcaron;</trd>,
    <trd>vyfukova&tcaron;</trd>,
    <trd>&ccaron;isti&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="vent.0umilo">
  <kap><tld/>umilo</kap>
  <snc mrk="vent.0umilo.TUA">
    <dif>
      Ilo a&ubreve; aparato por <tld/>umi, precipe portebla duondisko 
      refaldebla, kiun oni mane svingas:
      <ekz>
        <tld/>umilo el eburo;
      </ekz>
      <ekz>
        <tld/>umiloj el strutaj plumoj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la manoj kun na&gcirc;membrano dislar&gcirc;i&gcirc;is en formo de <tld/>umilo
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        grandaj <tld/>umilformaj palmoj staris gracie
        <fnt>
          <bib>Fab4</bib>
          <lok>Sterkskarabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#x2026;]</klr> ripozis en la ombro de l' granda <tld/>umilo,
        kiun tenis lia
        adjutanto
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">F&auml;cher</trd>
    <trd lng="pl">wachlarz</trd>
    <trd lng="ru">&c_v;&c_je;&c_je;&c_r;</trd>
  </snc>
  <snc mrk="vent.0umilo.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      <ref tip="dif" cel="ventol.0ilo.AGR">Ventolilo</ref>:
      <ekz>
        lia <tld/>umilo estas en lia mano, kaj li elpurigos sian
        dra&scirc;ejon
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 3:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel gren<tld/>umilo, oni <tld/>okribras la erarojn de
        aliuloj, 
        sed siajn proprajn erarojn oni ka&scirc;as
        <fnt><aut>trad. T. T. Anuruddha</aut>,
          <vrk>La Vojo al Nirvano a&ubreve; la Dhammapado, 18:252</vrk>,
          <lok>B&#x1ED3;-T&aacute;t-Csoma-Instituto por Budhologio, 1973</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Worfschaufel</trd>,
      <trd>Sichter</trd>
    </trdgrp>
    <trd lng="pl">wialnia</trd>
  </snc>
  <trd lng="bo">&#xF62;&#xFB3;&#xF74;&#xF44;&#xF0B;&#xF42;&#xF61;&#xF56;&#xF0B;</trd>
  <trd lng="cs">v&ecaron;j&iacute;&rcaron;</trd>
  <trdgrp lng="fr">
    <trd>&eacute;ventail</trd>,
    <trd>van</trd>
  </trdgrp>
  <trd lng="id">kipas</trd>
  <trdgrp lng="it">
    <trd>ventola</trd>,
    <trd>ventaglio</trd>,
    <trd>ventilabro</trd>
  </trdgrp>
  <trd lng="sk">vej&aacute;r</trd>
</drv>

<drv mrk="vent.0kapa">
  <kap><tld/>anima,
  <var><kap><tld/>kapa</kap></var></kap>
  <snc mrk="vent.0kapa.KOMUNE">
    <dif>
      Senpripensema, malsa&gcirc;a, sencerba:
      <ekz>
        malgra&ubreve; via tuta <tld/>animeco vi esence estas bona knabo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>animulo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>kapulo
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">windig <klr>(Charakter)</klr></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_je;&c_t;&c_r;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_l;&c_je;&c_g;&c_k;&c_a;&c_d;&c_u;&c_m;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">lehkomysln&yacute;</trd>
  <trdgrp lng="fr">
    <trd>&eacute;tourdi</trd>,
    <trd>fou-fou</trd>,
    <trd>t&ecirc;te en l'air</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szeles</trd>,
    <trd>hebehurgya</trd>
    <trd>szeleburdi</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>sconsiderato</trd>,
    <trd>stordito<klr> (sciocco)</klr></trd>,
    <trd>scervellato</trd>,
    <trd>sventato</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>lekkomy&sacute;lny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_je;&c_g;&c_k;&c_o;&c_m;&c_y;&c_s;&c_l;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_je;&c_t;&c_r;&c_je;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">&lcaron;ahkomyse&lcaron;n&yacute;</trd>
</drv>

<drv mrk="vent.dis0igxi">
  <kap>dis<tld/>umi, <var><kap>dis<tld/>igi</kap></var></kap>
  <snc mrk="vent.dis0igxi.KOMUNE">
    <dif>
      Dispeli, disperdigi:
      <ekz>
        ili &gcirc;in dis<tld/>umos, kaj dezertigos &gcirc;ian landon, &ccirc;ar en la
        tago de la
        malfeli&ccirc;o ili estos &ccirc;irka&ubreve; &gcirc;i &ccirc;iuflanke
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 51:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        re&gcirc;o <klr>[&#x2026;]</klr> dis<tld/>umas per siaj okuloj &ccirc;ion malbonan
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 20:8</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>zerstreuen</trd>,
      <trd>verwehen</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_mol;&c_v;&c_je;&c_ja;&c_c;&c_c;&c_a; &c_p;&c_a; &c_v;&c_je;&c_t;&c_r;&c_y;</trd>
  <trdgrp lng="fr">
    <trd>s'<ind>&eacute;parpiller</ind></trd>,
    <trd>se <ind>diss&eacute;miner</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&eacute;tsz&oacute;r&oacute;dik</trd>,
    <trd>sz&eacute;thullik</trd>
  </trdgrp>
  <trd lng="it">disperdersi</trd>
  <trd lng="pl">rozwiewa&cacute;</trd>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_v;&c_je;&c_ja;&c_t;&c_mol;&c_s;&c_ja; &c_p;&c_o; &c_v;&c_je;&c_t;&c_r;&c_u;</trd>
</drv>

<drv mrk="vent.dorso0o">
  <kap>dorso<tld/>o, <var><kap>dors<tld/>o</kap></var>
    <fnt>
      <aut>G. Waringhien</aut>:
      <vrk>Grand dictionnaire Esp&eacute;ranto-Fran&ccedil;ais</vrk>.
      <lok>Art. &leftquot;dorso&rightquot;</lok>
    </fnt>
  </kap>
  <snc mrk="vent.dorso0o.favora">
    <dif>
      Favora <tld/>o, <tld/>o blovanta samdirekte kun la movo de
      vel&scirc;ipo, avio ktp:
      <ekz>
        restis ankora&ubreve; 151&#x202F;km &gcirc;is tiu celo, se la dorsvento da&ubreve;ros,
        <klr>[&#x2026;]</klr> ni
        eble sukcesos
        <fnt>
          <vrk><url ref="https://bemi.tejo.org/2018/08/07/pliaj-bonkoraj-homoj/">Pliaj
          bonkoraj homoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <ctl>Sekreto</ctl> estis iranta kun duono de la veloj &ccirc;e
        dorso<tld/>o
        <fnt>
          <aut>A. Grin, tr. Jurij Finkel</aut>:
          <vrk>Skarlataj veloj</vrk>.
          <lok><url ref="http://esperanto-mv.narod.ru/Grin/skarlataj_05.html">&Ccirc;ap.
          5&#xAA;.</url></lok>
        </fnt>. 
      </ekz>
    </dif>
    <trd lng="de">R&uuml;ckenwind</trd>
  </snc>
  <trd lng="en">fair wind</trd>
  <trd lng="fr">vent arri&egrave;re</trd>
  <trd lng="ru">&c_p;&c_o;&c_p;&c_u;&c_t;&c_n;&c_y;&c_j; &c_v;&c_je;&c_t;&c_je;&c_r;</trd>
</drv>

<drv mrk="vent.0montrilo">
  <kap>
    <tld/>montrilo,
    <var><kap><tld/>omontrilo</kap></var>
  </kap>
  <snc mrk="vent.0montrilo.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Simpla meteologia instrumento me&hcirc;anika indikanta la direkton
      (kaj eventuale anka&ubreve; la rapidon) de vento, en formo de
      metala flageto (a&ubreve; koko, a&ubreve; alia figuro) turni&gcirc;anta sur
      vertikala akso, a&ubreve; en formo de teksa&jcirc;a maniko fiksita &ccirc;e
      turnebla ringo sur masto:
      <ekz>
        sed li sidis sur la <tld/>montrilo de la turo, &gcirc;i konstante
        turni&gcirc;adis kun li, kaj tial li pensis, ke &ccirc;iam blovas la
        sama <tld/>o
        <fnt><bib>Fab3</bib>,
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok></fnt>;
      </ekz><ekz>
        kurioza&jcirc;o estas sur la tegmento de la pre&gcirc;ejo: la
        <tld/>omontrilo konsistas el du simboloj: koko kutima
        franca simbolo kaj porkokapo, alia simbolo, da&ubreve;ra 
        memora&jcirc;o pri loka legendo
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Flagy_%28Seine-et-Marne%29">
          Flagy (Seine-et-Marne)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">v&ecaron;trn&aacute; korouhvi&ccaron;ka</trd>
  <trd lng="de">Windrichtungsgeber</trd>
  <trd lng="en">weather vane</trd>
  <trd lng="fr">girouette</trd>
  <trd lng="it">segnavento</trd>
  <trd lng="pl">wiatrowskaz</trd>
  <trd lng="ru">&c_f;&c_l;&c_ju;&c_g;&c_je;&c_r;</trd>
  <trd lng="sk">vetern&aacute; ru&zcaron;ica</trd>
</drv>

