<drv mrk="vent.0a">
  <kap><tld/>a</kap>
  <snc mrk="vent.0a.MET">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a vetero
        <fnt><bib>PatrojFiloj</bib>, <lok>&Ccirc;ap. 22a.</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a energio
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2005/008577.php">Venta
          energio superas la atoman</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a, sen&scirc;irma, senarba <klr>[&#8230;]</klr> insuleto
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2007/009255.php">Insulo
          de verkistoj</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_je;&c_t;&c_r;&c_a;&c_n;&c_y;</trd>
    <trd lng="id"><baz>angin</baz>berangin</trd>
    <trdgrp lng="it">
      <trd>ventoso</trd>,
      <trd>ventilato</trd>,
      <trd>eolico</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>wietrzny</trd>,
      <trd>wiatrowy</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_je;&c_t;&c_r;&c_o;&c_v;&c_o;&c_j;</trd>,
      <trd>&c_v;&c_je;&c_t;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="vent.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref cel="baz.sen0a">Senbaza</ref>,  <ref cel="firm.mal0a">malfirma</ref>,
      <ref cel="valor.sen0a">senvalora</ref>, <ref cel="aer.0a.malfirma">aera</ref>:
      <ekz>
        &ccirc;io estas vanta&jcirc;o kaj entreprenoj <tld/>aj
        <fnt>
          <bib>MT</bib>
          <lok>Predikanto 1:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;io estas vanta&jcirc;o kaj <tld/>a&jcirc;o, kaj nenia profito
        <fnt>
          <bib>MT</bib>
          <lok>Predikanto 2:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u sa&gcirc;ulo povas respondi per <tld/>aj opinioj?
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 15:2</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_l;&c_je;&c_g;&c_k;&c_a;&c_d;&c_u;&c_m;&c_n;&c_y;</trd>
    <trd lng="ru">&c_v;&c_je;&c_t;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_v;&c_je;&c_t;&c_r;&c_o;&c_v;&c_i;&c_t;</trd>,
    <trd>&c_v;&c_ja;&c_t;&c_malmol;&c_r;&c_n;&c_i;&c_ch;&c_a;&c_v;</trd>
  </trdgrp>
  <trd lng="cs">v&ecaron;trn&yacute;</trd>
  <trd lng="de">windig</trd>
  <trd lng="fr">fumeux</trd>
  <trdgrp lng="it">
    <trd>fumo<klr> (fig.)</klr></trd>,
    <trd>vento<klr> (fig.)</klr></trd>,
    <trd>aria<klr> (fig.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x98A8;&#x306E;  <pr>&#x304B;&#x305C;&#x306E;</pr></trd>,
    <trd>&#x98A8;&#x306E;&#12424;&#12358;&#x306A;  <pr>&#x304B;&#x305C;&#x306E;&#12424;&#12358;&#x306A;</pr></trd>,
    <trd>&#x7A7A;&#x865A;&#x306A;  <pr>&#x304F;&#12358;&#x304D;&#12423;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vetern&yacute;</trd>,
    <trd>vetrov&yacute;</trd>,
    <trd>vzdu&scaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_t;&c_r;&#x43E;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_ib;&c_t;&c_r;&#x44F;&#x43D;&c_i;&c_j; <klr>(&#x43F;&c_r;&c_i;&#x43A;&#x43C;. &c_d;&#x43E; &c_v;&c_ib;&c_t;&c_je;&c_r;)</klr></trd>,
    <trd>&c_v;&c_ib;&c_t;&c_r;&#x44F;&#x43D;&c_i;&c_j; <klr>(&#x44F;&#x43A;&c_i;&c_j; &c_d;&c_ib;&c_jeu; &c_z;&c_a;
    &c_d;&#x43E;&#x43F;&#x43E;&#x43C;&#x43E;&c_g;&#x43E;&#x44E; &c_v;&c_ib;&c_t;&c_r;&c_u;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="vent.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="vent.0i.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Blovi <klr>(pp <tld/>o)</klr>:
      <ekz>
        forte <tld/>as<fnt>Z</fnt>;
      </ekz>
      <ekz>
        el tombo de prakatakombo plor<tld/>as kantik'
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga
          Butiko</url></vrk>
          <lok>A&ubreve;tunovesper'</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_mol;&c_m;&c_u;&c_c;&c_mol;
    <klr tip="amb">(&c_p;&c_r;&c_a; &c_v;&c_je;&c_c;&c_je;&c_r;)</klr></trd>
  <trdgrp lng="cs">
    <trd>b&yacute;t v&ecaron;trno</trd>,
    <trd>foukat</trd>,
    <trd>vanout</trd>
  </trdgrp>
  <trd lng="de">wehen <klr>(Wind)</klr></trd>
  <trd lng="fr">venter</trd>
  <trd lng="hu">f&uacute;j a <ind>sz&eacute;l</ind></trd>
  <trd lng="id"><baz>embus</baz>berembus</trd>
  <trd lng="it">soffiare<klr> (di vento)</klr></trd>
  <trd lng="ja">&#x98A8;&#x304C;&#21561;&#x304F;  <pr>&#x304B;&#x305C;&#x304C;&#12405;&#x304F;</pr></trd>
  <trd lng="nl">waaien</trd>
  <trd lng="pl">wia&cacute; <klr>(o wietrze)</klr></trd>
  <trd lng="pt">ventar</trd>
  <trd lng="ru">&c_d;&c_u;&c_t;&c_mol; <klr tip="amb">(&c_b;&c_je;&c_z;&c_l;. &c_o;
&c_v;&c_je;&c_t;&c_r;&c_je;)</klr></trd>
  <trdgrp lng="sk">
    <trd>f&uacute;ka&tcaron;</trd>,
    <trd>via&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_u;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_ib;&#x44F;&c_t;&c_i; <klr>(&#x43F;&c_r;&#x43E; &c_v;&c_ib;&c_t;&c_je;&c_r;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="vent.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="vent.0ego.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Fortega <tld/>o, ordinare akompanata de pluvo a&ubreve; ne&gcirc;o:
      <ekz>
        en tiu nokto blovis terura <tld/>ego kaj la pluvo faladis per
        riveroj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiel pasanta <tld/>ego <klr>[la]</klr> malpiulo malaperas
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 10:25</lok>
        </fnt>;
      </ekz><ekz>
        kvietigi la <tld/>egon
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 107:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        malfeli&ccirc;o venos kiel <tld/>ego
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 1:27</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tempes.0o">tempesto</ref>,
      <ref cel="uragan.0o">uragano</ref>
    </refgrp>
    <trdgrp lng="pl">
      <trd>wicher</trd>,
      <trd>wichura</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>badai</trd>,
      <trd>topan</trd>,
      <trd><ind>angin</ind> ribut</trd>,
      <trd>lesus</trd>,
      <trd>puyuh</trd>,
      <trd><ind>puting</ind> beliung</trd>
    </trdgrp>
  </snc>
  <snc mrk="vent.0ego.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Rapide trapasanta amaso:
      <ekz>
        la <tld/>egoj de la vivo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la mevoj flirtis &ccirc;irka&ubreve; li kvaza&ubreve; <tld/>ego da
        blankaj flugiloj;
      </ekz><ekz>
        li devis elporti <tld/>egon da ripro&ccirc;oj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lavang.0o">lavango</ref>,
      <ref cel="torent.0o">torento</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_u;&c_r;&c_a;&c_g;&c_a;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>vichr</trd>,
    <trd>vich&rcaron;ice</trd>
  </trdgrp>
  <trd lng="de">Sturm</trd>
  <trd lng="el">&alfa;&ny;&epsilon;&my;&omikron;&zeta;&alfa_ton;&lambda;&eta;</trd>
  <trd lng="fr">temp&ecirc;te</trd>
  <trdgrp lng="hu">
    <trd>vihar</trd>,
    <trd>sz&eacute;lvihar</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>tempesta</trd>,
    <trd>tormenta</trd>
  </trdgrp>
  <trdgrp lng="ja">
