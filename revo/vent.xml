<drv mrk="vent.dis0igxi">
  <kap>dis<tld/>umi, <var><kap>dis<tld/>igi</kap></var></kap>
  <snc mrk="vent.dis0igxi.KOMUNE">
    <dif>
      Dispeli, disperdigi:
      <ekz>
        ili &gcirc;in dis<tld/>umos, kaj dezertigos &gcirc;ian landon, &ccirc;ar en la
        tago de la
        malfeli&ccirc;o ili estos &ccirc;irka&ubreve; &gcirc;i &ccirc;iuflanke
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 51:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        re&gcirc;o <klr>[&#8230;]</klr> dis<tld/>umas per siaj okuloj &ccirc;ion malbonan
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 20:8</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>zerstreuen</trd>,
      <trd>verwehen</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_mol;&c_v;&c_je;&c_ja;&c_c;&c_c;&c_a; &c_p;&c_a; &c_v;&c_je;&c_t;&c_r;&c_y;</trd>
  <trdgrp lng="fr">
    <trd>s'<ind>&eacute;parpiller</ind></trd>,
    <trd>se <ind>diss&eacute;miner</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&eacute;tsz&oacute;r&oacute;dik</trd>,
