<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lar.xml,v 1.30 2024/01/15 16:14:08 revo Exp $">
<kap>
  <rad>lar</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="lar.0o">
  <kap><tld/>o</kap>
  <snc mrk="lar.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="lared.0oj"><tld/>edoj</ref>
      (<trd lng="la">Larus</trd>), t.e. <ref tip="super" cel="mev.0o">mevoj</ref>:
      <ekz>
        &ccirc;e la kamparo kaj la okcidenta maro <klr>[&#x2026;]</klr> a&ubreve;di&gcirc;as la kriego de mevo kaj
        <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2010/010133.php">&Scirc;an&gcirc;i&gcirc;anta mondo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/b/ba/Larus_michahellis_Olbia.jpg/320px-Larus_michahellis_Olbia.jpg" prm="CC BY-SA 3.0">Flavkrura <tld/>o (Larus michahellis), Sardinio, Italujo
        <fnt>
          <aut>Gzzz, propra verko</aut>
          <lok><url ref="&WCW;/File:Larus_michahellis_Olbia.jpg">&WCW;/File:Larus_michahellis_Olbia.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <ref tip="hom" cel="lar1.0o">laro</ref>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_r;&c_l;&c_ja;</trd>,
    <trd>&c_ch;&c_a;&c_j;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>gavi&agrave;</trd>,
    <trd>gavina</trd>,
    <trd>gavinot</trd>
  </trdgrp>
  <trd lng="cs">racek <klr>(mo&rcaron;sk&yacute;)</klr></trd>
  <trd lng="de">M&ouml;we</trd>
  <trd lng="en">gull</trd>
  <trd lng="fr">go&eacute;land</trd>
  <trd lng="hu">sir&aacute;ly</trd>
  <trd lng="ja">&#x30AB;&#x30E2;&#x30E1;</trd>
  <trdgrp lng="nl">
    <trd>larus</trd>,
    <trd>meeuw</trd>
  </trdgrp>
  <trd lng="pl">mewa</trd>
  <trdgrp lng="pt">
    <trd>gaivota</trd>,
    <trd>gaivot&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_a;&c_j;&c_k;&c_a;</trd>
  <trd lng="sk">&ccaron;ajka morsk&aacute;</trd>
  <trd lng="uk">&#x43C;&c_a;&c_r;&c_t;&c_i;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x9DD7; <pr>&#x14D;u</pr></trd>,
    <trd>&#x9E25; <pr>&#x14D;u</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lar.0oformaj">
  <kap><tld/>oformaj</kap>
  <snc mrk="lar.0oformaj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref> el la klaso <ref tip="malprt" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Lariformes</trd>).
    </dif>
  </snc>
  <trd lng="ca">caradriformes</trd>
  <trd lng="de">M&ouml;wenv&ouml;gel</trd>
  <trd lng="hu">sir&aacute;lyalak&uacute;ak</trd>
  <trdgrp lng="nl">
    <trd>lariformes</trd>,
    <trd>meeuwen<klr tip="ind"> (orde)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_zh;&c_a;&c_n;&c_k;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;</trd>
  <trd lng="uk">&#x43C;&c_a;&c_r;&c_t;&c_i;&#x43D;&#x43E;&#x43F;&#x43E;&c_d;&c_ib;&c_b;&#x43D;&c_ib;</trd>
</drv>
</art>
