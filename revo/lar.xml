<vortaro>
<art mrk="$Id$">
<kap>
<rad>lar</rad>/o
</kap>
<drv mrk="lar.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="lared.0oj"><tld/>edoj</ref>
      (<trd lng="la">Larus</trd>).
    </dif>
  </snc>
  <trd lng="de">M&ouml;we</trd>
  <trd lng="nl">meeuw</trd>
</drv>

<drv mrk="lar.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Lariformes</trd>).
    </dif>
  </snc>
  <trd lng="de">M&ouml;venv&ouml;gel</trd>
</drv>
</art>
