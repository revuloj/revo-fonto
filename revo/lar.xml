<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>lar</rad>/o
</kap>
<drv mrk="lar.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="lar.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Larus</trd>).
    </dif>
  </snc>
  <trd lng="de">M&ouml;we</trd>
</drv>
<drv mrk="lar.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="lar.0oformaj"><tld/>oformaj
      birdoj</ref> (<trd lng="la">Laridae</trd>).
    </dif>
    <ref tip="prt" cel="mev.0o">mevo</ref>
  </snc>
  <trd lng="de">M&ouml;wen <klr tip="ind">(Familie)</klr></trd>
</drv>
<drv mrk="lar.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Lariformes</trd>).
    </dif>
  </snc>
  <trd lng="de">M&ouml;venv&ouml;gel</trd>
</drv>
</art>
