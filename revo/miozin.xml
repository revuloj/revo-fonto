<art mrk="$Id: miozin.xml,v 1.7 2024/01/26 21:21:13 revo Exp $">
<kap>
  <rad>miozin</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="miozin.0o">
  <kap><tld/>o</kap>
  <snc mrk="miozin.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      <ref tip="super" cel="protein.0o.FIZL">Proteino</ref>
      interalie rolanta en la stre&ccirc;o de muskoloj:
      <ekz>
        <tld/>o: muskola proteino apartenanta al la globulinoj
        <fnt>
          <aut>D. Portmann, D. Pogers</aut>,
          <vrk><url ref="&STEB;kemio/doug-portmann/kemio-glosaro.htm">
          Glosaro</url></vrk>,
          <lok>en: Kemia kaj sciencteknika vortaro, 1993</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_ja;&c_z;&c_y;&c_n;</trd>
  <trd lng="de">Myosin</trd>
  <trd lng="en">myosin</trd>
  <trd lng="fr">myosine</trd>
  <trd lng="uk">&#x43C;&c_ib;&#x43E;&c_z;&c_i;&#x43D;</trd>
  <trd lng="zh">&#x808C;&#29699;&#x86CB;&#x767D; <pr>j&#x12B;qi&#xFA;d&#xE0;nb&#xE1;i</pr></trd>
</drv>
</art>

