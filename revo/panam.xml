<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>Panam</rad>/o
</kap>

<drv mrk="panam.P0o">
  <kap>
    <ofc>8</ofc><tld lit="P"/>o
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/aktoj/aktoj2/8oa.html"> 8a
Oficiala Aldono</url></vrk>, <lok>II.C.1</lok></fnt>
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>
    <fnt><bib>Viki</bib>, <lok><url 
        ref="http://eo.wikipedia.org/wiki/Panamo">Panamo</url></lok></fnt>
  </kap>
  <snc mrk="panam.P0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o">&Scirc;tato</ref> en la centra 
      <ref tip="malprt" cel="amerik.A0o">Ameriko</ref>
      kun &ccirc;efurbo Panamurbo.
      Teritorio de Respubliko Panamo.
      ISO-3166 kodo: PA.
      &Gcirc;i limas kun
      <ref tip="vid" cel="kolombi.K0o">Kolombio</ref>,
      <ref tip="vid" cel="kostarik.0o">Kostariko</ref>
      kaj oceanoj:
      <ref tip="vid" cel="atlant.A0o">Atlantiko</ref>
      kaj <ref tip="vid" cel="pacifik.P0o">Pacifiko</ref>.
    </dif>
    </snc>
    <trd lng="be">&c_P;&c_a;&c_n;&c_a;&c_m;&c_a;</trd>
    <trd lng="bg">&c_P;&c_a;&c_n;&c_a;&c_m;&c_a;</trd>
    <trd lng="de">Panama</trd>
    <trd lng="en">Panama</trd>
    <trd lng="es">Panam&aacute;</trd>
  <trd lng="fr">Panama</trd>
    <trd lng="id">Panama</trd>
    <trd lng="nl">Panama</trd>
    <trdgrp lng="pl">
      <trd>Panama</trd>,
      <trd>Republika <ind>Panamy</ind></trd>
    </trdgrp>
    <trd lng="pt">Panam&aacute;</trd>
</drv> 

<drv mrk="panam.0ano">
  <kap><tld lit="p"/>ano,
  <var><kap><tld lit="p"/>anino</kap></var></kap>
  <snc>
    <dif>
    Civitano, civitanino de Panamo.
    </dif>
  </snc>
  <trdgrp lng="fr"> 
    <trd>Panam&eacute;en</trd>
    <trd>Panam&eacute;enne<klr> (subst.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="panam.0a">
  <kap><tld lit="p"/>a</kap>
  <snc>
    <dif>
    Rilata al la &scirc;tato Panamo.
    </dif>
  </snc>
  <trdgrp lng="fr"> 
    <trd>panam&eacute;en</trd>
    <trd>panam&eacute;enne<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>

</art>

