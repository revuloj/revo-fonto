<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: panam.xml,v 1.15 2019/10/21 18:10:24 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>Panam</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="panam.P0o">
  <kap>
    <ofc>8</ofc><tld lit="P"/>o
    <fnt><bib>RLN</bib></fnt>
  </kap>
  <mlg kod="ISO-3166">PA</mlg>
  <snc mrk="panam.P0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&Scirc;tato</ref> en la
      centra <ref tip="malprt" cel="amerik.A0o">Ameriko</ref>; teritorio
      de Respubliko Panamo, limanta kun <ref tip="vid"
      cel="kolombi.K0o">Kolombio</ref>, <ref tip="vid"
      cel="kostarik.0o">Kostariko</ref> kaj oceanoj: <ref tip="vid"
      cel="atlant.A0o">Atlantiko</ref> kaj <ref tip="vid"
      cel="pacifik.P0o">Pacifiko</ref>; kun samnoma &ccirc;efurbo:
      <ekz>
        amba&ubreve; gepatroj de <nom>McCain</nom> ja estis civitanoj de Usono,
        sed
        li naski&gcirc;is en hospitalo en <tld/>o kaj teorie povus fari&gcirc;i
        civitano de <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Alexander Gofen</aut>
          <vrk><url ref="&Monato;/2009/009811.php">Obamena&jcirc;o, a&ubreve; haladzo
          de socialisma aventurismo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la plej ri&ccirc;aj landoj en la mondo ur&gcirc;e postulis reformojn en
        landoj <klr>[&#8230;]</klr>
        kiuj disponigas nepravigeblajn impostajn faciligojn,
        <klr>[&#8230;]</klr>
        en la listo estas anka&ubreve; landoj kiel Andoro, Monako kaj Panamo,
        <klr>[&#8230;]</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url
          ref="http://steloj.de/esperanto/monato2/007128.html">&Ccirc;u venos
          fino de la impostaj paradizoj?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      &Ccirc;ef<ref tip="lst" cel="urb.0o" lst="voko:urboj">urbo</ref> de la
      &scirc;tato <ref tip="malprt" cel="panam.P0o.GEOG"><tld/>o</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_P;&c_a;&c_n;&c_a;&c_m;&c_a;</trd>
  <trd lng="bg">&c_P;&c_a;&c_n;&c_a;&c_m;&c_a;</trd>
  <trd lng="de">Panama</trd>
  <trd lng="en">Panama</trd>
  <trd lng="es">Panam&aacute;</trd>
  <trd lng="fr">Panama</trd>
  <trd lng="id">Panama</trd>
  <trd lng="nl">Panama</trd>
  <trd lng="pl">Panama</trd>
  <trd lng="pt">Panam&aacute;</trd>
</drv> 

<drv mrk="panam.0a">
  <kap><tld lit="p"/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        la projektegoj de la Sueza kaj <tld/>a kanaloj
        <klr>[estis]</klr> fruaj
        provoj de tutmondigo, kiuj komence anka&ubreve; fiaskis kaj ekfunkciis
        nur longe post ekkonstruado
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url
          ref="&Monato;/2008/009502.php">Gigantismo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">panamaisch</trd>
  <trd lng="en">Panamanian</trd>
  <trdgrp lng="fr"> 
    <trd>panam&eacute;en</trd>
    <trd>panam&eacute;enne<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="nl">Panamees</trd>
</drv>

<drv mrk="panam.0ano">
  <kap><tld lit="p"/>ano</kap>
  <snc>
    <dif>
      Civitano de Panamo:
      <ekz>
        oni diras, ke por <tld lit="p"/>ano nenio estas pli serioza
        afero ol karnavalo
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref="https://eo.wikipedia.org/?curid=17552">Karnavalo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Panamaer</trd>
  <trd lng="en">Panamanian</trd>
  <trdgrp lng="fr"> 
    <trd>Panam&eacute;en</trd>
    <trd>Panam&eacute;enne<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="nl">Panamees</trd>
</drv>
</art>

