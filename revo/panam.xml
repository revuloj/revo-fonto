<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>panam</rad>/o
</kap>

<drv mrk="panam.0o">
  <kap>
    <tld lit="P"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="panam.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o">&Scirc;tato</ref> en la centra 
      <ref tip="malprt" cel="amerik.A0o">Ameriko</ref>
      kun &ccirc;efurbo Panamurbo.
      Teritorio de Respubliko Panamo.
      ISO-3166 kodo: PA.
      &Gcirc;i limas kun
      <ref tip="vid" cel="kolombi.0o">Kolombio</ref>,
      <ref tip="vid" cel="kostarik.0o">Kostariko</ref>
      kaj oceanoj:
      <ref tip="vid" cel="atlant.0o">Atlantiko</ref>
      kaj <ref tip="vid" cel="pacifik.0o">Pacifiko</ref>.
    </dif>
    <trd lng="bg">&c_P;&c_a;&c_n;&c_a;&c_m;&c_a;</trd>
    <trd lng="de">Panama</trd>
    <trd lng="en">Panama</trd>
    <trd lng="nl">Panama</trd>
    <trdgrp lng="pl">
      <trd>Panama</trd>,
      <trd>Republika <ind>Panamy</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

