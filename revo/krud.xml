<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>krud</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="krud.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="krud.0a.KOMUNE">
    <dif>
      Tia, kia la naturo &gcirc;in produktis,
      konservanta sian primitivan staton;
    </dif>
    <subsnc mrk="krud.0a.neprilaborita">
      <dif>
        Neprilaborita, nepreparita, nekulturita:
      <ekz>
        <tld/>a lano, silko, pelto<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a ledo, marmoro <klr>(nepolurita)</klr>;
      </ekz>
      <ekz>
        <tld/>a diamanto  <klr>(ne facetita)</klr>;
      </ekz>
      <ekz>
        <tld/>a sukero <klr>(ne rafinita)</klr>;
      </ekz>
      <ekz>
        <tld/>a tolo <klr>(ne ankora&ubreve; lavita)</klr>;
      </ekz>
      <ekz>
        <tld/>a fero <klr>(fandfero)</klr>;
      </ekz>
      <ekz>
        <tld/>a grundo <klr>(ne plugita)</klr>;
      </ekz>
      <ekz>
        en unu parto pendis feloj kaj ha&ubreve;toj, <tld/>aj kaj 
        tanitaj
        <fnt><bib>Fab3</bib>, <lok>Kora sufero</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>a karno <klr>(de malsanulo)</klr> estas malpura:
        &gcirc;i estas lepro
        <fnt><bib>MT</bib>, <lok>&Lev; 13:15</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[la odoro de floroj]</klr> estis pli kaj pli sentebla
        inter        
        la <tld/>aj odoroj de la tero kaj akvo
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 16</lok></fnt>;
      </ekz>
      <ekz>
        nuda kaj <tld/>a, sen gro&scirc;o en
        po&scirc;o<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la montoj aspektas pli <tld/>e<fnt>Z</fnt>.
      </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="krud.0a.KUI">
      <uzo tip="fak">KUI</uzo>       
      <dif>
        Nekuirita:
        <ekz>
          <tld/>a frukto; 
        </ekz>
        <ekz>
          <tld/>a viando;
        </ekz>
        <ekz>
          ne man&gcirc;u &gcirc;in duon<tld/>an, nek kuiritan en akvo,
          sed nur rostitan sur fajro
          <fnt><bib>MT</bib>, <lok>&Eli; 12:9</lok></fnt>;
        </ekz>
      </dif>
      <trd lng="id">mentah</trd>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="sovagx.0a.KOMUNE">sova&gcirc;a<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_s;&c_y;&c_r;&c_y;</trd>,
      <trd>&c_n;&c_je;&c_a;&c_p;&c_r;&c_a;&c_c;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
     <trd>unbearbeitet</trd>,
     <trd>nat&uuml;rlich</trd>,
     <trd>naturbelassen</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>natural</trd>,
      <trd>primitive</trd>,
      <trd>raw</trd>,
      <trd>nugget (p.p oro)</trd>,
      <trd>unbleached (p.p. lano)</trd>,
      <trd>uncooked (p.p frukto, viando)</trd>,
      <trd>uncut (p.p. diamanto)</trd>,
      <trd>undressed (p.p. ledo, fero, silko)</trd>,
      <trd>virgin (p.p. tero)</trd>
    </trdgrp>
    <trd lng="nl">ruw</trd>
    <trdgrp lng="pl">
     <trd>surowy</trd>,
     <trd>nieprzetworzony</trd>,
     <trd>naturalny</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>cru</trd>,
      <trd>natural</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_y;&c_r;&c_o;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_o;&c_b;&c_r;&c_a;&c_b;&c_o;&c_t;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="krud.0a.FIG">
    <dif>
      <ref tip="vid" cel="mild.mal0a.KOMUNE">Malmilda</ref>,
      abrupta.
    </dif>
    <subsnc mrk="krud.0a.akra">
      <dif>
        Forte kaj malagrable impresanta la sentumojn pro neperfekta
        laboriteco a&ubreve; pro ne sufi&ccirc;a harmonio;
        neharmonia, maldelikata:
        <ekz>
          <tld/>a koloro, kontrasto, vino, vo&ccirc;o;
        </ekz>
        <ekz>
          la kolero igas la viza&gcirc;on pli <tld/>a;
        </ekz>
        <ekz>
          <tld/>a skizo, stilo, vorto, kanto;
        </ekz>
        <ekz>
          en la sono de lia vo&ccirc;o estis a&ubreve;debla io <tld/>a,
          preska&ubreve; malrespekta<fnt>B</fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_g;&c_r;&c_u;&c_b;&c_y;</trd>
      <trdgrp lng="de">
       <trd>unharmonisch</trd>
      </trdgrp>
      <trd lng="nl">ruw</trd>
      <trdgrp lng="pl">
        <trd>surowy</trd>
      </trdgrp>
      <trd lng="ru">&c_g;&c_r;&c_u;&c_b;&c_y;&c_j;</trd>
    </subsnc>
    <subsnc mrk="krud.0a.intensa">
      <dif>
        Dolorige impresanta la sentumojn
        pro bruta fortego a&ubreve; sova&gcirc;a
        intenseco; pene tolerebla, malmildega:
        <ekz>
          la homoj devas konstante batali kontra&ubreve; la <tld/>aj
          fortoj de la naturo<fnt>Z</fnt>;
        </ekz>
        <ekz>
          <tld/>a aero de vintro premas &ccirc;ion<fnt>B</fnt>;
        </ekz>
        <ekz>
          <tld/>a vintra tago<fnt>Z</fnt>;
        </ekz>
        <ekz>
          &ccirc;io ku&scirc;is kovrita de glacio kaj ne&gcirc;o,
          kaj <tld/>a vento tran&ccirc;is al &ccirc;iu la viza&gcirc;on
          <fnt><bib>Fab3</bib>, <lok>Historio de unu
          patrino</lok></fnt>;
        </ekz>
        <ekz>
          <tld/>a kolero, vorto, movo, pu&scirc;o;
        </ekz>
        <ekz>
          la vetero estis <tld/>a, la &ccirc;ielo griza, malklara kaj
          peza<fnt>Z</fnt>;
        </ekz>
        <ekz>
          la <tld/>a tumulto de la batalo<fnt>Z</fnt>;
        </ekz>
        <ekz>
          &ccirc;ion <tld/>e rompi<fnt>Z</fnt>;
        </ekz>
        <ekz>
          ne tro multe, ne tro <tld/>e per unu fojo<fnt>Z</fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_g;&c_r;&c_u;&c_b;&c_y;</trd>
      <trdgrp lng="de">
       <trd>wild</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>streng</trd>,
        <trd>rauw</trd>
      </trdgrp>
      <trdgrp lng="pl">
       <trd>surowy</trd>,
       <trd>kanciasty</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_g;&c_r;&c_u;&c_b;&c_y;&c_j;</trd>,
        <trd>&c_s;&c_u;&c_r;&c_o;&c_v;&c_y;&c_j;</trd>
      </trdgrp>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="sovagx.0a.FIG">sova&gcirc;a<sncref/></ref>
    </refgrp>
  </snc>
  <snc mrk="krud.0a.MOR">
    <dif>
      Ne mildigita de civilizita kulturo; brute instinkta; malklera,
      mal&gcirc;entilega, vulgaranima:
      <ekz>
        li <klr>[...]</klr> dis&scirc;iris siajn vestojn kaj dancis nude
        anta&ubreve; la <tld/>aj sova&gcirc;aj soldatoj
        <fnt><bib>Fab4</bib>, <lok>Malbona princo</lok></fnt>;
      </ekz>
      <ekz>
        la sento de hereda aristokrato koncerne <tld/>an
        kampulon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a karaktero, ruza&jcirc;o, insulto, parolo, eraro;
      </ekz>
      <ekz>
        plej <tld/>a supersti&ccirc;o sufokas ilin<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_g;&c_r;&c_u;&c_b;&c_y;</trd>,
      <trd>&c_v;&c_u;&c_l;&c_mol;&c_g;&c_a;&c_r;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
     <trd>wild</trd>,
     <trd>ungez&uuml;gelt</trd>,
     <trd>vulg&auml;r</trd>
    </trdgrp>
    <trd lng="nl">onbehouwen</trd>
    <trdgrp lng="pl">
     <trd>niekrzesany</trd>,
      <trd>prostacki</trd>,
       <trd>ordynarny</trd>
    </trdgrp>
    <trd lng="ru">&c_g;&c_r;&c_u;&c_b;&c_y;&c_j;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>divoký</trd>,
    <trd>drsný</trd>,
    <trd>hrubý</trd>,
    <trd>surový <klr>(materiál)</klr></trd>
  </trdgrp>
  <trd lng="de">roh</trd>
  <trdgrp lng="es">
    <trd>crudo</trd>,
    <trd>bruto</trd>,
    <trd>tosco</trd>,
    <trd>rudimentario</trd>,
    <trd>r&uacute;stico</trd>,
    <trd>virgen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&agrave; <ind>vif</ind><klr tip="ind"> (en p. de
chair)</klr></trd>,
    <trd>brut</trd>,
    <trd>cru<klr tip="ind"> (non cuit, brut, sans nuance,
    grossier, froid)</klr></trd>,
    <trd>&eacute;cru</trd>,
    <trd>grossier<klr tip="ind"> (fruste, impoli, brut)</klr></trd>,
    <trd>rudimentaire</trd>,
    <trd>rustique</trd>
  </trdgrp>
  <trdgrp lng="hu">
   <trd>nyers</trd>,
   <trd>durva</trd>,
   <trd>faragatlan</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>divoký</trd>,
    <trd>drsný</trd>,
    <trd>hrubý</trd>,
    <trd>surový</trd>
  </trdgrp>
</drv>

<drv mrk="krud.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="krud.0ajxo.maldelikata">
    <dif>
      Io <tld/>a:
      <ekz>
        &ccirc;iujn malsa&gcirc;a&jcirc;ojn kaj <tld/>a&jcirc;ojn de
        la popolo li batadis
        <fnt><bib>Fab2</bib>, <lok>Dano Holger</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_g;&c_r;&c_u;&c_b;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    <trd lng="es">groser&iacute;a</trd>
    <trd lng="fr">grossi&egrave;ret&eacute;</trd>
    <trdgrp lng="hu">
     <trd>nyerses&eacute;g</trd>,
     <trd>durvas&aacute;g</trd>
    </trdgrp>
    <trd lng="nl"><ind>ruw</ind> materiaal</trd>
    <trd lng="ru">&c_g;&c_r;&c_u;&c_b;&c_o;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="krud.0ajxo.materialo">
    <ref tip="dif" cel="materi1.krud0o.EKON"><tld/>materialo</ref>
    <trd lng="be">&c_s;&c_y;&c_r;&c_a;&c_v;&c_ib;&c_n;&c_a;</trd>
    <trd lng="de">Rohmaterial</trd>
    <trd lng="es"><ind>materia</ind> prima</trd>
    <trd lng="fr"><ind>mati&egrave;re</ind> premi&egrave;re</trd>
    <trd lng="hu">nyersanyag</trd>
    <trd lng="nl">grondstof</trd>
    <trdgrp lng="pl">
     <trd>prostactwo</trd>,
     <trd>nieokrzesanie</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_y;&c_r;&c_mol;&c_jo;</trd>
  </snc>
</drv>

<drv mrk="krud.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="krud.0ulo.MOR">
    <dif>
      Malbone edukita homo:
      <ekz>
        se vi <tld/>ulon iom karesos,
        li al si &ccirc;ion permesos
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        plej kuirita kampulo restos &ccirc;iam <tld/>ulo
        <fnt>Z</fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="delika.mal0ulo">maldelikatulo</ref>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_u;&c_b;&c_ib;&c_ja;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>hrubián</trd>,
    <trd>hulvát</trd>,
    <trd>neomalenec</trd>
  </trdgrp>
  <trd lng="de">Rohling</trd>
  <trdgrp lng="es">
    <trd>paleto</trd>,
    <trd>pat&aacute;n</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>rustaud</trd>,
    <trd>rustre</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nyers ember</trd>,
    <trd>faragatlan tusk&oacute;</trd>,
    <trd>bunk&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>boerenhufter</trd>,
    <trd>boerenkinkel</trd>,
    <trd>boerenpummel</trd>,
    <trd>botterik</trd>,
    <trd>ongelikte beer</trd>,
    <trd>lomperd</trd>,
    <trd>hork</trd>,
    <trd>hufter</trd>,
    <trd>kinkel</trd>, 
    <trd>onbeschofterik</trd>, 
    <trd>palurk</trd>, 
    <trd>plebejer</trd>, 
    <trd>plomperd</trd>, 
    <trd>pummel</trd>
  </trdgrp>
  <trdgrp lng="pl">
   <trd>prostak</trd>,
   <trd>nieokrzesaniec</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_b;&c_i;&c_ja;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>grobian</trd>,
    <trd>hrubian</trd>,
    <trd>surovec</trd>
  </trdgrp>
</drv>

<drv mrk="krud.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="krud.mal0a.KOMUNE">
    <dif>
      Delikata.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_o;&c_n;&c_k;&c_ib;</trd>,
    <trd>&c_v;&c_y;&c_t;&c_a;&c_n;&c_ch;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_d;&c_a;&c_l;&c_ib;&c_k;&c_a;&c_n;&c_t;&c_y;</trd>,
    <trd>&c_r;&c_a;&c_f;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>upravený</trd>,
    <trd>vyčištěný</trd>,
    <trd>zjemnělý</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>bearbeitet</trd>,
     <trd>verfeinert</trd>,
     <trd>raffiniert</trd>
  </trdgrp>
  <trd lng="es">refinado</trd>
  <trd lng="fr">raffin&eacute;</trd>
  <trd lng="hu">finom</trd>
  <trd lng="nl">geraffineerd</trd>
  <trdgrp lng="pl">
    <trd>delikatny</trd>,
    <trd>przetworzony</trd>,
    <trd>wyrafinowany</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_l;&c_i;&c_k;&c_a;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_u;&c_t;&c_o;&c_n;&c_ch;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_o;&c_n;&c_k;&c_i;&c_j;</trd>,
<trd>
&c_r;&c_a;&c_f;&c_i;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;
</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hebký</trd>,
    <trd>jemný</trd>
  </trdgrp>
</drv>
</art>
