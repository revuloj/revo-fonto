<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: krud.xml,v 1.24 2021/06/18 15:33:47 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>krud</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="krud.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="krud.0a.KOMUNE">
    <dif>
      Tia, kia la naturo &gcirc;in produktis,
      konservanta sian <ref tip="sin" cel="primit.0a">primitiva</ref>n staton.
    </dif>
    <subsnc mrk="krud.0a.neprilaborita">
      <dif>
        Ne<ref cel="labor.pri0i">prilabori</ref>ta, nepreparita, nekulturita:  
        <ekz>
          tenejoj de <tld/>aj materialoj
          <fnt>
            <bib>Far1</bib>
            <lok>&Ccirc;apitro XIX</lok>
          </fnt>;
        </ekz>
        <ekz>
          en unu parto pendis feloj kaj ha&ubreve;toj, <tld/>aj kaj 
          tanitaj
          <fnt><bib>Fab3</bib>, <lok>Kora sufero</lok></fnt>;
        </ekz>
        <ekz>
          persono, envolvita en blanka <tld/>a pelto
          <fnt>
            <bib>Fab2</bib>
            <lok>Ne&gcirc;a re&gcirc;ino</lok>
          </fnt>;
        </ekz>
        <ekz>
          la <tld/>a karno <klr>(de malsanulo)</klr> estas malpura:
          &gcirc;i estas lepro
          <fnt><bib>MT</bib>, <lok>&Lev; 13:15</lok></fnt>;
        </ekz>
        <ekz>
          <klr>[la odoro de floroj]</klr> estis pli kaj pli sentebla
          inter        
          la <tld/>aj odoroj de la tero kaj akvo
          <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 16</lok></fnt>;
        </ekz>
        <ekz>
          nuda kaj <tld/>a, sen gro&scirc;o en
          po&scirc;o<fnt><bib>PrV</bib></fnt>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="krud.0a.KUI">
      <uzo tip="fak">KUI</uzo>       
      <dif>
        Ne<ref cel="kuir.0i">kuiri</ref>ta:
        <ekz>
          <tld/>a frukto; 
        </ekz>
        <ekz>
          sin nutradi je kampaj herboj kaj <tld/>a viando
          <fnt>
            <bib>LR</bib>
            <lok>4. Fonetiko</lok>
          </fnt>;
        </ekz>
        <ekz>
          ne man&gcirc;u &gcirc;in duon<tld/>an, nek kuiritan en akvo,
          sed nur rostitan sur fajro
          <fnt><bib>MT</bib>, <lok>&Eli; 12:9</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="id">mentah</trd>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="sovagx.0a.KOMUNE">sova&gcirc;a<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_s;&c_y;&c_r;&c_y;</trd>,
      <trd>&c_n;&c_je;&c_a;&c_p;&c_r;&c_a;&c_c;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
     <trd>unbearbeitet</trd>,
     <trd>nat&uuml;rlich</trd>,
     <trd>naturbelassen</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>natural</trd>,
      <trd>primitive</trd>,
      <trd>raw</trd>,
      <trd>nugget <klr>(pp oro)</klr></trd>,
      <trd>unbleached <klr>(pp lano)</klr></trd>,
      <trd>uncooked <klr>(pp frukto, viando)</klr></trd>,
      <trd>uncut <klr>(pp diamanto)</klr></trd>,
      <trd>undressed <klr>(pp ledo, fero, silko)</klr></trd>,
      <trd>virgin <klr>(pp tero)</klr></trd>
    </trdgrp>
    <trd lng="nl">ruw</trd>
    <trdgrp lng="pl">
     <trd>surowy</trd>,
     <trd>nieprzetworzony</trd>,
     <trd>naturalny</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>cru</trd>,
      <trd>natural</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_y;&c_r;&c_o;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_o;&c_b;&c_r;&c_a;&c_b;&c_o;&c_t;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="krud.0a.FIG">
    <dif>
      <ref tip="sin" cel="mild.mal0a.KOMUNE">Malmilda</ref>,
      <ref tip="sin" cel="abrupt.0a">abrupta</ref>.
    </dif>
    <subsnc mrk="krud.0a.akra">
      <dif>
        Forte kaj malagrable impresanta la sentumojn pro ne<ref tip="ant" cel="perfek.0a">perfekta</ref>
        laboriteco a&ubreve; pro ne sufi&ccirc;a harmonio; ne<ref tip="ant" cel="harmon1.0a">harmonia</ref>, 
        mal<ref tip="ant" cel="delika.0a">delikata</ref>:
        <ekz>
          la virino en la funebra vesto kaptis tiun <tld/>an manon kaj premis &gcirc;in forte
          <fnt>
            <bib>Marta</bib>
          </fnt>;
        </ekz>
        <ekz>
          via desegno estas preska&ubreve; tute kovrita per tro <tld/>aj strekoj de krajono
          <fnt>
            <bib>Marta</bib>
          </fnt>;
        </ekz>
        <ekz>
          tiama portreto, iom <tld/>a kaj maldol&ccirc;a, montras la mal&gcirc;ojon de l' gimnaziano
          <fnt>
            <bib>VivZam</bib>
          </fnt>;
        </ekz>
        <ekz>
          <tld/>a silko po 6 &scirc;ilingoj
          <fnt>
            <bib>FK</bib>
          </fnt>;
        </ekz>
        <ekz>
          Vi tiam portis lanfulardon / Sur <tld/>a bluzo
          <fnt>
            <aut>Raymond Schwartz</aut>
            <vrk><url ref="http://steloj.de/esperanto/butiko">La Stranga Butiko</url></vrk>
            <lok>Profitulo</lok>
          </fnt>;
        </ekz>
        <ekz>
          stranga kokokrio <klr>[&#8230;]</klr> <tld/>a, mankis delikateco
          <fnt>
            <bib>Fab4</bib>
            <lok>Korta koko kaj ventkoko</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_g;&c_r;&c_u;&c_b;&c_y;</trd>
      <trdgrp lng="de">
       <trd>unharmonisch</trd>
      </trdgrp>
      <trd lng="nl">ruw</trd>
      <trdgrp lng="pl">
        <trd>surowy</trd>
      </trdgrp>
      <trd lng="ru">&c_g;&c_r;&c_u;&c_b;&c_y;&c_j;</trd>
    </subsnc>
    <subsnc mrk="krud.0a.intensa">
      <dif>
        <ref tip="sin" cel="dolor.0iga">Dolorige</ref> impresanta la sentumojn 
        pro bruta fortego a&ubreve; sova&gcirc;a intenseco; pene 
        <ref tip="ant" cel="toler.0ebla">tolerebla</ref>, malmildega:
        <ekz>
          <tld/>a vintra tago
          <fnt>
            <bib>BdV</bib>
          </fnt>;
        </ekz>
        <ekz>
          <tld/>a vento tran&ccirc;is al &ccirc;iu la viza&gcirc;on
          <fnt>
            <bib>Fab3</bib>
            <lok>Historio de unu patrino</lok>
          </fnt>;
        </ekz>
        <ekz>
          <tld/>aj, malsekaj a&ubreve;tunaj nebuloj
          <fnt>
            <bib>Fab4</bib>
            <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
          </fnt>;
        </ekz>
        <ekz>
          &ccirc;i tie interne &ccirc;io estis malagrabla, <tld/>a kaj &scirc;ima
          <fnt>
            <bib>Fab4</bib>
            <lok>Historio el la dunoj</lok>
          </fnt>;
        </ekz>
        <ekz>
          ili vivas <klr>[&#8230;]</klr> kontra&ubreve; tiuj <tld/>aj fortoj de la naturo, kiuj ilin
          &ccirc;iujn egale atakas
          <fnt>
            <bib>Paroloj</bib>
            <vrk>Parolado en la Guildhall (Londono) en la 21a de a&ubreve;gusto 1907</vrk>
          </fnt>;
        </ekz>
        <ekz>
          la <tld/>a tumulto de la batalo
          <fnt>
            <bib>Rabistoj</bib>
            <lok>Akto Unua</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_g;&c_r;&c_u;&c_b;&c_y;</trd>
      <trdgrp lng="de">
       <trd>wild</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>streng</trd>,
        <trd>rauw</trd>
      </trdgrp>
      <trdgrp lng="pl">
       <trd>surowy</trd>,
       <trd>kanciasty</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_g;&c_r;&c_u;&c_b;&c_y;&c_j;</trd>,
        <trd>&c_s;&c_u;&c_r;&c_o;&c_v;&c_y;&c_j;</trd>
      </trdgrp>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="sovagx.0a.FIG">sova&gcirc;a<sncref/></ref>
    </refgrp>
  </snc>
  <snc mrk="krud.0a.MOR">
    <dif>
      Ne mildigita de <ref tip="ant" cel="civili.0ita">civilizita</ref> kulturo; 
      brute instinkta; <ref tip="sin" cel="kler.mal0a">malklera</ref>,
      mal&gcirc;entilega, <ref tip="sin" cel="vulgar.0a.maldelikata">vulgar</ref>anima:
      <ekz>
        li <klr>[...]</klr> dis&scirc;iris siajn vestojn kaj dancis nude
        anta&ubreve; la <tld/>aj sova&gcirc;aj soldatoj
        <fnt><bib>Fab4</bib>, <lok>Malbona princo</lok></fnt>;
      </ekz>
      <ekz>
        delikata besto inter la <tld/>aj soldatoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>!
      </ekz>
      <ekz>
        frato, <tld/>a kaj sova&gcirc;a <klr>[&#8230;]</klr> jam en la infanaj ludoj ne lasis al ni
        pacon kaj konsenton
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a besta <klr>(<ref cel="brutal.0a.perforta">brutala</ref>)</klr> forto, 
        por kiu ekzistas nur la demando &leftquot;kiu kie super kiu
        <klr>[&#8230;]</klr> devas regi&rightquot;
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Homaranismo</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
       <ref cel="barbar.0a">barbara</ref>,
       <ref cel="kultur.sen0a">senkultura</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_g;&c_r;&c_u;&c_b;&c_y;</trd>,
      <trd>&c_v;&c_u;&c_l;&c_mol;&c_g;&c_a;&c_r;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
     <trd>wild</trd>,
     <trd>ungez&uuml;gelt</trd>,
     <trd>vulg&auml;r</trd>
    </trdgrp>
    <trd lng="nl">onbehouwen</trd>
    <trdgrp lng="pl">
     <trd>niekrzesany</trd>,
      <trd>prostacki</trd>,
       <trd>ordynarny</trd>
    </trdgrp>
    <trd lng="ru">&c_g;&c_r;&c_u;&c_b;&c_y;&c_j;</trd>
  </snc>
  <trd lng="bo">&t_ra;&t_jata;&t_drengbu;&t_na;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>divok&yacute;</trd>,
    <trd>drsn&yacute;</trd>,
    <trd>hrub&yacute;</trd>,
    <trd>surov&yacute; <klr>(materi&aacute;l)</klr></trd>
  </trdgrp>
  <trd lng="de">roh</trd>
  <trdgrp lng="es">
    <trd>crudo</trd>,
    <trd>bruto</trd>,
    <trd>tosco</trd>,
    <trd>rudimentario</trd>,
    <trd>r&uacute;stico</trd>,
    <trd>virgen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&agrave; <ind>vif</ind><klr tip="ind"> (en p. de
chair)</klr></trd>,
    <trd>brut</trd>,
    <trd>cru<klr tip="ind"> (non cuit, brut, sans nuance,
    grossier, froid)</klr></trd>,
    <trd>&eacute;cru</trd>,
    <trd>grossier<klr tip="ind"> (fruste, impoli, brut)</klr></trd>,
    <trd>rudimentaire</trd>,
    <trd>rustique</trd>
  </trdgrp>
  <trdgrp lng="hu">
   <trd>nyers</trd>,
   <trd>durva</trd>,
   <trd>faragatlan</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>divok&yacute;</trd>,
    <trd>drsn&yacute;</trd>,
    <trd>hrub&yacute;</trd>,
    <trd>surov&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="krud.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="krud.0ajxo.maldelikata">
    <dif>
      Io <tld/>a:
      <ekz>
        <klr>[ili]</klr> trovis pli kaj pli da <tld/>a&jcirc;o-receptoj, e&ccirc; por pico, supo kaj
        torto
        <fnt>
          <bib>Monato</bib>
          <aut>Lode van de Velde</aut>
          <vrk><url ref="https://www.monato.be/2004/008405.html">Kuirita tomato estas morta tomato</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iujn malsa&gcirc;a&jcirc;ojn kaj <tld/>a&jcirc;ojn de
        la popolo li batadis
        <fnt><bib>Fab2</bib>, <lok>Dano Holger</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a&jcirc;oj metitaj en la bu&scirc;ojn de la personoj kaj scenoj, kiuj malfaciligus la
        &gcirc;uadon de tiu verko
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_g;&c_r;&c_u;&c_b;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    <trd lng="es">groser&iacute;a</trd>
    <trd lng="fr">grossi&egrave;ret&eacute;</trd>
    <trdgrp lng="hu">
     <trd>nyerses&eacute;g</trd>,
     <trd>durvas&aacute;g</trd>
    </trdgrp>
    <trd lng="nl"><ind>ruw</ind> materiaal</trd>
    <trd lng="ru">&c_g;&c_r;&c_u;&c_b;&c_o;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="krud.0ajxo.materialo">
    <dif>
      <ref tip="dif" cel="materi1.krud0o.EKON"><tld lit="K"/>materialo</ref>:
      <ekz>
        la produktado de varoj <klr>[&#8230;]</klr> estis praktikata kun granda konsumo de
        <tld/>a&jcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007100.html">Kiu sukcesis, kiu postrestis?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_y;&c_r;&c_a;&c_v;&c_ib;&c_n;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Rohmaterial</trd>,
      <trd>Rohstoff</trd>
    </trdgrp>
    <trd lng="es"><ind>materia</ind> prima</trd>
    <trd lng="fr"><ind>mati&egrave;re</ind> premi&egrave;re</trd>
    <trd lng="hu">nyersanyag</trd>
    <trd lng="nl">grondstof</trd>
    <trdgrp lng="pl">
     <trd>prostactwo</trd>,
     <trd>nieokrzesanie</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_y;&c_r;&c_mol;&c_jo;</trd>
  </snc>
</drv>

<drv mrk="krud.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="krud.0ulo.MOR">
    <dif>
      <tld lit="K"/>a homo:
      <ekz>
        ke tio povus esti nobla, intence farita sinofero, neniel venis en la kapon de la
        <tld/>ulo
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Foster</nom> estis <tld/>ulo needukita, kaj li ofte pritraktis temojn tute erare
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u oni nomas <tld/>uloj la laboristojn a&ubreve; ne, tio indiferentu al ili.
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        plej kuirita kampulo &ccirc;iam restos <tld/>ulo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        se vi <tld/>ulon iom karesos,
        li al si &ccirc;ion permesos
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="delika.mal0ulo">maldelikatulo</ref>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_u;&c_b;&c_ib;&c_ja;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>hrubi&aacute;n</trd>,
    <trd>hulv&aacute;t</trd>,
    <trd>neomalenec</trd>
  </trdgrp>
  <trd lng="de">Rohling</trd>
  <trdgrp lng="es">
    <trd>paleto</trd>,
    <trd>pat&aacute;n</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>rustaud</trd>,
    <trd>rustre</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nyers ember</trd>,
    <trd>faragatlan tusk&oacute;</trd>,
    <trd>bunk&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>boerenhufter</trd>,
    <trd>boerenkinkel</trd>,
    <trd>boerenpummel</trd>,
    <trd>botterik</trd>,
    <trd>ongelikte beer</trd>,
    <trd>lomperd</trd>,
    <trd>hork</trd>,
    <trd>hufter</trd>,
    <trd>kinkel</trd>, 
    <trd>onbeschofterik</trd>, 
    <trd>palurk</trd>, 
    <trd>plebejer</trd>, 
    <trd>plomperd</trd>, 
    <trd>pummel</trd>
  </trdgrp>
  <trdgrp lng="pl">
   <trd>prostak</trd>,
   <trd>nieokrzesaniec</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_b;&c_i;&c_ja;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>grobian</trd>,
    <trd>hrubian</trd>,
    <trd>surovec</trd>
  </trdgrp>
</drv>

<drv mrk="krud.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="krud.mal0a.KOMUNE">
    <dif>
      Prilaborita, kuirita, <ref tip="sub" cel="delika.0a">delikata</ref>...:
      <ekz>
        <klr>[tiu &scirc;tato]</klr> praktikas al la E&ubreve;ropa Unio la kiom eble plej mal<tld/>an
        influon
        <fnt>
          <bib>MD14</bib>
          <vrk>Germanio, modelaj kli&scirc;oj</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="fajn.0a">fajna</ref>,
      <ref cel="sublim.0aa">sublima</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_o;&c_n;&c_k;&c_ib;</trd>,
    <trd>&c_v;&c_y;&c_t;&c_a;&c_n;&c_ch;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_d;&c_a;&c_l;&c_ib;&c_k;&c_a;&c_n;&c_t;&c_y;</trd>,
    <trd>&c_r;&c_a;&c_f;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>upraven&yacute;</trd>,
    <trd>vy&ccaron;i&scaron;t&ecaron;n&yacute;</trd>,
    <trd>zjemn&ecaron;l&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>bearbeitet</trd>,
    <trd>verfeinert</trd>,
    <trd>raffiniert</trd>,
