<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: statur.xml,v 1.16 2020/04/08 07:10:18 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>statur</rad>/o <fnt><bib>MT</bib></fnt>
</kap>
<drv mrk="statur.0o">
  <kap><tld/>o</kap>
  <snc mrk="statur.0o.KOMUNE">
    <dif>
      Alteco kaj imponeco de la homa korpo:
      <ekz>
        mi ne silentos pri &gcirc;iaj membroj, pri &gcirc;ia forto kaj bela <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 41:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        via <tld/>o similas la palmotrunkon
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 7:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ne havis <tld/>on nek belecon
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 53:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        aldoni unu ulnon al sia <tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 6:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        Jesuo progresis en sa&gcirc;eco kaj <tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>Luko 2:52</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis malgranda je <tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>Luko 19:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de re&gcirc;ino
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        delikata, kun nobla <tld/>o kaj bela viza&gcirc;o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>bon<tld/>a</ind>
        <fnt>
          <bib>Marta</bib>
        </fnt>.
        <trd lng="be">&c_s;&c_a;&c_m;&c_a;&c_v;&c_ib;&c_t;&c_y;</trd>
        <trd lng="de">gut gebaut <klr>(Statur)</klr></trd>
      </ekz>
    </dif>
    <ref tip="sin" cel="kresk.0o.KOMUNE">kresko</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_n;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_f;&c_ib;&c_g;&c_u;&c_r;&c_a;</trd>,
    <trd>&c_r;&c_o;&c_s;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>postava</trd>,
    <trd>vzr&uring;st</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Statur</trd>,
    <trd>Gr&ouml;&szlig;e</trd>,
    <trd>Gestalt</trd>
  </trdgrp>
  <trd lng="en">stature</trd>
  <trdgrp lng="fr">
    <trd>prestance</trd>,
    <trd>stature</trd>
  </trdgrp>
  <trd lng="hu">termet</trd>
  <trdgrp lng="nl">
    <trd>postuur</trd>,
    <trd>lichaamsbouw</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_s;&c_a;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_n;</trd>,
    <trd>&c_f;&c_i;&c_g;&c_u;&c_r;&c_a;</trd>,
    <trd>&c_r;&c_o;&c_s;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>postava</trd>,
    <trd>vzrast</trd>
  </trdgrp>
</drv>

<drv mrk="statur.bel0a">
  <kap>bel<tld/>a</kap>
  <snc mrk="statur.bel0a.KOMUNE">
    <dif>
      Havanta iom altan korpon kaj agrablajn proporciojn de membroj:
      <ekz>
        la junulino estis bel<tld/>a
        <fnt>
          <bib>MT</bib>
          <lok>Ester 2:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        vestita je parada uniformo, li estis bel<tld/>a kaj tre
        imponanta
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>,
        <lok>&ccirc;apitro 18a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>wohlstatuiert</trd>,
      <trd>wohlgestaltet</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">kr&aacute;sn&eacute; postavy <klr>(adj.)</klr></trd>
  <trdgrp lng="fr">
    <trd>qui a de l'<ind>allure</ind></trd>,
    <trd>de belle <ind>prestance</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>deli</trd>,
    <trd>d&eacute;lceg</trd>,
    <trd>sz&eacute;p <ind>termet&udblac;</ind>&gt;</trd>
  </trdgrp>
  <trd lng="sk">kr&aacute;snej postavy</trd>
</drv>
</art>
