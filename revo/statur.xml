<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>statur</rad>/o
</kap>
<drv mrk="statur.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Alteco de la homa korpo:
      <ekz>
        bon<tld/>a<fnt>Z</fnt>;
        <trd lng="be">&c_s;&c_a;&c_m;&c_a;&c_v;&c_ib;&c_t;&c_y;</trd>
      </ekz>
      <ekz>
        bel<tld/>a homo<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_n;</trd>, 
    <trd>&c_p;&c_o;&c_s;&c_t;&c_a;&c_c;&c_mol;</trd>, 
    <trd>&c_f;&c_ib;&c_g;&c_u;&c_r;&c_a;</trd>, 
    <trd>&c_r;&c_o;&c_s;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Statur</trd>,
    <trd>Gr&ouml;&szlig;e</trd>,
    <trd>Gestalt</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>postuur</trd>,
    <trd>lichaamsbouw</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_s;&c_a;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_n;</trd>,
    <trd>&c_f;&c_i;&c_g;&c_u;&c_r;&c_a;</trd>,
    <trd>&c_r;&c_o;&c_s;&c_t;</trd>
  </trdgrp>
</drv>
</art>
