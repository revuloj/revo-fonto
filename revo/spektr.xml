<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>spektr</rad>/o
</kap>
<drv mrk="spektr.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Fantomo.
    </dif>
    <trdgrp lng="de">
      <trd>Gespenst</trd>,
      <trd>Geist</trd>,
      <trd>Spukgestalt</trd>,
      <trd>Phantom</trd>,
      <trd>Scheinbild</trd>,
      <trd>Trugbild</trd>
    </trdgrp>
    <trd lng="hu">k&iacute;s&eacute;rtet</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Homo granda, maldika kaj malgrasa.
    </dif>
    <trdgrp lng="de">
      <trd>Bohnenstange</trd>,
      <trd>langer <ind>Lulatsch</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>h&oacute;rihorgas</trd>,
      <trd>langal&eacute;ta</trd></trdgrp>
  </snc>
  <snc mrk="spektr.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Vico da diversaj koloraj radioj, rezultanta el malkomponi&gcirc;o
      de la lumo.
    </dif>
    <trd lng="be">&c_s;&c_p;&c_e;&c_k;&c_t;&c_a;&c_r;</trd>
    <trd lng="de">Spektrum</trd>
    <trdgrp lng="hu">
      <trd>sz&iacute;nk&eacute;p</trd>,
      <trd>spektrum</trd></trdgrp>
    <trd lng="ru">&c_s;&c_p;&c_je;&c_k;&c_t;&c_r;</trd>
  </snc>
  <snc mrk="spektr.0o.ajgeno">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <dif>
      <klr>(de <ref cel="endomo.0o">endomorfio</ref> en
      <ref cel="spac.vektora0o">vektora spaco</ref>)</klr>
      La aro de &gcirc;iaj <ref tip="prt" cel="ajgen.0o">ajgenoj</ref>.
    </dif>
    <trd lng="be">&c_s;&c_p;&c_e;&c_k;&c_t;&c_a;&c_r;</trd>
    <trd lng="de">Spektrum</trd>
    <trd lng="en">spectrum</trd>
    <trd lng="hu">spektrum</trd>
    <trdgrp lng="pl">
      <trd>spektrum</trd>,
      <trd>widmo</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_p;&c_je;&c_k;&c_t;&c_r;</trd>
  </snc>
  <trd lng="fr">spectre</trd>
  <trd lng="nl">spectrum</trd>
  <adm>
    PIV1 &scirc;ajne ne plu konas la sencojn 1 kaj 2, sed konas figuran
    sencon iom samsencan kiel "serio", "vico"... <aut>MB</aut>
  </adm>
</drv>
</art>
