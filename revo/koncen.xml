<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>8</ofc>
      <rad>koncentr</rad>/i
    </kap>

    <drv mrk="koncen.0i">
      <kap><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
	<dif>
	  Kunigi dense &ccirc;irka&ubreve; komuna centro; alcentrigi:
	  <ekz>
	    <tld/>i trupojn, &scirc;ipojn;
	  </ekz><ekz>
	    <tld/>ado de kapitalo;
	  </ekz><ekz>
	    <uzo tip="stl">FIG</uzo>
	    <tld/>i sian atenton;
	  </ekz><ekz>
	    <uzo tip="stl">FIG</uzo>
	    ni <tld/>u niajn klopodojn kaj penadojn sur la junularon;
	  </ekz><ekz>
	    <tld/>ita solva&jcirc;o.
	  </ekz>
	</dif>
	<refgrp tip="vid">
	  <ref cel="kolekt.0i">kolekti</ref>,
	  <ref cel="akumul.0i">akumuli</ref>,
	  <ref cel="fokus">fokusigi</ref>.
	</refgrp>
      </snc>
      <trd lng="be">&c_k;&c_a;&c_n;&c_c;&c_e;&c_n;&c_t;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
      <trd lng="en">concentrate</trd>
      <trd lng="de">konzentrieren</trd>
      <trdgrp lng="hu">
        <trd>&ouml;sszpontos&iacute;t</trd>,
        <trd>koncentr&aacute;l</trd>
      </trdgrp>
      <trd lng="nl">concentreren</trd>
      <trdgrp lng="ru">
	<trd>&c_s;&c_o;&c_s;&c_r;&c_je;&c_d;&c_o;&c_t;&c_o;&c_ch;&c_i;&c_t;&c_mol;</trd>,
	<trd>&c_k;&c_o;&c_n;&c_c;&c_je;&c_n;&c_t;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
    </drv>

    <drv mrk="koncen.0igxi">
      <kap><tld/>i&gcirc;i</kap>
      <snc>
	<dif>
	  Kuni&gcirc;i dense &ccirc;irka&ubreve; komuna centro:
	  <ekz>
	    la lo&gcirc;antoj de tero iom post iom <tld/>i&gcirc;os en la regionoj
	    subekvatoraj<fnt>Z</fnt>;
	  </ekz><ekz>
	    &ccirc;iuj rigardoj <tld/>i&gcirc;is al li;
	  </ekz><ekz>
	    tie <tld/>i&gcirc;is la tuta komerco;
	  </ekz><ekz>
	    en tiu oficejo <tld/>i&gcirc;as la laboroj<fnt>Z</fnt>;
	  </ekz><ekz>
	    <uzo tip="stl">FIG</uzo>
	    komence liaj pensoj <tld/>i&gcirc;is sur la mono<fnt>B</fnt>.
	  </ekz>
	</dif>
      </snc>
      <trd lng="be">&c_k;&c_a;&c_n;&c_c;&c_e;&c_n;&c_t;&c_r;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
      <trd lng="de">sich <ind>konzentrieren</ind></trd>
      <trdgrp lng="en">
	<trd>concentrate</trd>,
	<trd>become concentrated</trd>
    </trdgrp>
      <trdgrp lng="hu">
        <trd>&ouml;sszpontosul</trd>,
        <trd>koncentr&aacute;l&oacute;dik</trd>
      </trdgrp>
      <trd lng="nl">zich concentreren</trd>
      <trd lng="ru">&c_k;&c_o;&c_n;&c_c;&c_je;&c_n;&c_t;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </drv>

    <drv mrk="koncen.0a">
      <kap><tld/>a</kap>
      <snc mrk="koncen.0a.ada">
	<dif>
	  Rilata al <tld/>ado:
	  <ekz>
	    la <tld/>a funkcio de la viva substanco;
	  </ekz><ekz>
	    <tld/>aj plioj galvanaj;
	  </ekz><ekz>
	    <tld/>a procezo, fluo.
	  </ekz>
	</dif>
    <trdgrp lng="hu">
      <trd>&ouml;sszpontos&iacute;t&oacute;</trd>,
      <trd>koncentr&aacute;l&oacute;</trd>
    </trdgrp>
	<trd lng="ru">&c_k;&c_o;&c_n;&c_c;&c_je;&c_n;&c_t;&c_r;&c_a;&c_c;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
      </snc>

      <snc mrk="koncen.0a.samcentra">
	<uzo tip="stl">ARK</uzo>
	<dif>
	  = <ref tip="dif" cel="centr.sam0a">Samcentra</ref>:
	  <ekz>
	    du <tld/>aj rondoj<fnt>B</fnt>
	  </ekz>
	</dif>
	<rim num="1">
	  La Scienca Revuo, la Teknika Komisiono (Scienca Gazeto, dec. 1913)
	  kaj la kompetentaj teknikistoj V&eacute;rax kaj W&uuml;ster unuvo&ccirc;e
	  rimarkigas, ke la adjektiva radiko <tld/>a<sncref
	    ref="koncen.0a.samcentra"/>, malprave
	  enkondukita de la 1a aldono, estas tute samsignifa, kiel samcentra,
	  kaj sekve tute senutila. Kontra&ubreve;e estus tre ta&ubreve;ga la verba radiko
	  <em>koncentri</em>, kun la signifo &leftquot;plidensigi solva&jcirc;on, pliigi
	  la kvanton de solvita substanco en solva&jcirc;o&rightquot;.
	  <aut>Noto el PV</aut>.
	</rim>
	<rim num="2">
	  Sekve, tiu senco jam estas arkaika, kaj &gcirc;i estas
	  eksoficialigita; PIV2 &gcirc;in indikas kiel neoficialan
	  homonimon, senrilate al la plidensiga <em>koncentr-</em>.
	  <aut>Sergio Pokrovskij</aut>
	</rim>
      </snc>
    </drv>

    <drv mrk="koncen.0o">
      <kap><tld/>o</kap>
      <snc mrk="koncen.0o.ado">
	<dif>
	  <tld lit="K"/>ado a&ubreve; <tld/>i&gcirc;o, ago <tld/>i a&ubreve; <tld/>i&gcirc;i..
	</dif>
      </snc>
      <snc mrk="koncen.0o.iteco">
	<dif>
	  <tld lit="K"/>iteco:
	  <ekz>
	    impona verko malgra&ubreve; la simpligoj kaj pro la forta <tld/>o
	    <fnt>
	      <aut>Poul THORSEN</aut>:
	      <vrk>La oceano kaj ni &dash; La arto de Mikines</vrk>.
	      <lok><url
ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/np/np5603/mikinesu.html"
		  >Norda Prismo, 1956:3</url></lok>
	    </fnt>
	  </ekz>
	</dif>
      </snc>
      <trd lng="ru">&c_k;&c_o;&c_n;&c_c;&c_je;&c_n;&c_t;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
      <trdgrp lng="hu">
        <trd>koncentr&aacute;ci&oacute;</trd>,
        <trd>&ouml;sszpontos&iacute;t&aacute;s</trd>,
        <trd>&ouml;sszpontosul&aacute;s</trd>
      </trdgrp>
    </drv>

    <drv mrk="koncen.mal0i">
      <kap>mal<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
	<dif>
	  Disigi, maldensigi, fari malpli <tld/>ita,
	  <ref tip="sub" cel="dilu.0i">dilui</ref>:
	  <ekz>
	    la&ubreve; iuj fakuloj, preferindas <tld/>i kapitalon por
	    disvolvi la lokon, kie estas la tri havenurboj, ol
	    mal<tld/>i &gcirc;in aparte por &gcirc;iu el ili
	    <fnt>
	      <aut>Wei Guoxiong</aut>: <vrk>Trafikreto en Guan&gcirc;i</vrk>
	      <lok><url
	    ref="http://mf1.iist.unu.edu/china/ElPopolaCxinio/bd98-12-19.html"
		  >EP&Ccirc;</url></lok></fnt>.
	  </ekz>
	</dif>
      </snc>
      <trdgrp lng="hu">
        <trd>sz&eacute;tforg&aacute;csol</trd>,
        <trd>fellaz&iacute;t</trd>,
        <trd>felh&iacute;g&iacute;t</trd>
      </trdgrp>
      <trdgrp lng="ru">
	<trd>&c_r;&c_a;&c_s;&c_s;&c_r;&c_je;&c_d;&c_o;&c_t;&c_o;&c_ch;&c_i;&c_t;&c_mol;</trd>,
	<trd>&c_u;&c_m;&c_je;&c_n;&c_mol;&c_sh;&c_i;&c_t;&c_mol; &c_k;&c_o;&c_n;&c_c;&c_je;&c_n;&c_t;&c_r;&c_a;&c_c;&c_i;&c_ju;</trd>
      </trdgrp>
    </drv>

    <drv mrk="koncen.0iteco">
      <kap><tld/>iteco</kap>
      <snc>
	<dif>
	  Grado, je kiu io estas <tld/>ita:
	  <ekz>
	    Vitamino <ctl>C</ctl> post aplikado en la samaj kvantoj
	    havas pli malaltan <tld/>itecon en la sango &ccirc;e
	    tabakfumantoj ol &ccirc;e tiuj kiuj ne fumas
	    <fnt>
	      <aut>Tr. Gabor Lazar</aut>:
	      <vrk>Fumado kaj la metabolo de kelkaj medikamentoj</vrk>.
	      <lok><url ref="http://www.eventoj.hu/arkivo/eve-207.htm"
		  >Eventoj, n-ro 207</url></lok>
	    </fnt>
	  </ekz>
	</dif>
      </snc>
      <trd lng="hu">koncentr&aacute;ci&oacute;</trd>
      <trd lng="ru">&c_k;&c_o;&c_n;&c_c;&c_je;&c_n;&c_t;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
    </drv>

    <drv mrk="koncen.0ejo">
      <kap><tld/>ejo</kap>
      <snc>
	<dif>
	  Barakaro a&ubreve; tendaro en kiu homoj estas <tld/>itaj,
	  kunigitaj dense &ccirc;irka&ubreve; komuna centro, a&ubreve;
	  por faciligi ilian proviantadon, a&ubreve; por amaspunadi ilin:
	  <ekz>
	    oni akceptis la viktimojn de la tertremo en apuda
	    <tld/>ejo;
	    <fnt><bib>ManPol</bib></fnt>
	  </ekz><ekz>
	    dum la milito milionoj mortis en la <tld/>ejoj.
	    <fnt><bib>ManPol</bib></fnt>
	  </ekz>
	</dif>
      </snc>
      <trdgrp lng="be">
	<trd>&c_k;&c_a;&c_n;&c_c;&c_e;&c_n;&c_t;&c_r;&c_a;&c_c;&c_y;&c_j;&c_n;&c_y; &c_l;&c_ja;&c_g;&c_je;&c_r;</trd>,
	<trd>&c_k;&c_a;&c_n;&c_c;&c_l;&c_ja;&c_g;&c_je;&c_r;</trd>
      </trdgrp>
      <trd lng="de">Konzentrationslager</trd>
      <trd lng="en">concentration camp</trd>
      <trd lng="es">campo de <ind>concentraci&oacute;n</ind></trd>
      <trd lng="fr">camp de <ind>concentration</ind></trd>
      <trd lng="hu">koncentr&aacute;ci&oacute;s t&aacute;bor</trd>
      <trd lng="nl">concentratiekamp</trd>
      <trd lng="pt">campo de <ind>concentrac&atilde;o</ind></trd>
      <trd lng="ru">&c_k;&c_o;&c_n;&c_c;&c_je;&c_n;&c_t;&c_r;&c_a;&c_c;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j; &c_l;&c_a;&c_g;&c_je;&c_r;&c_mol;</trd>
    </drv>
  </art>
