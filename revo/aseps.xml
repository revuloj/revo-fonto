<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aseps.xml,v 1.19 2024/01/03 09:26:57 revo Exp $">
<kap>
  <rad>aseps</rad>/o
  <fnt>Z</fnt>
</kap>

<drv mrk="aseps.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Procedo per kiu oni evitas en kirurgio la alporton de
      infektantaj &gcirc;ermoj.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_e;&c_p;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="cs">asepse</trd>
  <trd lng="de">Asepsis</trd>
  <trd lng="en">asepsis</trd>
  <trd lng="es">asepsia</trd>
  <trd lng="fr">asepsie</trd>
  <trdgrp lng="hu">
    <trd>aszepszis</trd>,
    <trd>cs&iacute;ramentes kezel&eacute;s</trd>
  </trdgrp>
  <trd lng="it">asepsi</trd>
  <trdgrp lng="ja">
    <trd>&#28961;&#x83CC;&#x6CD5;  <pr>&#12416;&#x304D;&#12435;&#x307B;&#12358;</pr></trd>,
    <trd>&#38450;&#33104;&#x6CD5;  <pr>&#x307C;&#12358;&#12405;&#x307B;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_s;&c_je;&c_p;&c_t;&c_i;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>asepsa</trd>,
    <trd>proti infek&#x10D;n&#xE1; prevencia</trd>
  </trdgrp>
</drv>

<drv mrk="aseps.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Bazita sur <tld/>o:
      <ekz>
        <tld/>a metodo.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Libera de mikroboj:
      <ekz>
        <tld/>a instrumento, vundo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_e;&c_p;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="cs">aseptick&#xFD;</trd>
  <trd lng="de">aseptisch</trd>
  <trd lng="fr">aseptique</trd>
  <trdgrp lng="hu">
    <trd>aszeptikus</trd>,
    <trd>cs&iacute;ramentes</trd>
  </trdgrp>
  <trd lng="it">asettico</trd>
  <trdgrp lng="ja">
    <trd>&#28961;&#x83CC;&#x6CD5;&#x306E;  <pr>&#12416;&#x304D;&#12435;&#x307B;&#12358;&#x306E;</pr></trd>,
    <trd>&#28961;&#x83CC;&#x306E;  <pr>&#12416;&#x304D;&#12435;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_s;&c_je;&c_p;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">aseptick&yacute;</trd>
  <trd lng="uk">&c_a;&c_s;&c_je;&#x43F;&c_t;&c_i;&c_ch;&#x43D;&c_i;&c_j;</trd>
</drv>

<drv mrk="aseps.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Uzi <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_je;&c_z;&c_z;&c_a;&c_r;&c_a;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_e;&c_z;&c_y;&c_n;&c_f;&c_e;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_e;&c_r;&c_y;&c_l;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_m;&c_e;&c_d;&c_y;&c_ch;&c_n;&c_y;&c_ja; &c_ib;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_e;&c_n;&c_t;&c_y;)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pou&#x17E;&#xED;t asepsi</trd>,
    <trd>zbavit n&#xE1;kazy</trd>
  </trdgrp>
  <trd lng="en">asepsis</trd>
  <trd lng="fr">aseptiser</trd>
  <trdgrp lng="hu">
    <trd>fert&odblac;tlen&iacute;t</trd>,
    <trd>cs&iacute;r&aacute;tlan&iacute;t</trd>
  </trdgrp>
  <trd lng="ja">&#28961;&#x83CC;&#x51E6;&#x7F6E;&#12377;&#x308B;  <pr>&#12416;&#x304D;&#12435;&#12375;&#12423;&#12385;&#12377;&#x308B;</pr></trd>
  <trd lng="sk">zbavi&tcaron; n&#xE1;kazy</trd>
</drv>
</art>
