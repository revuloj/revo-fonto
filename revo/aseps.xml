<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>aseps</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="aseps.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Procedo per kiu oni evitas en kirurgio la alporton de
      infektantaj &gcirc;ermoj.
    </dif>
  </snc>
  <trd lng="fr">asepsie</trd>
  <trd lng="ru">&c_a;&c_s;&c_je;&c_p;&c_t;&c_i;&c_k;&c_a;</trd>
</drv>

<drv mrk="aseps.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Bazita sur <tld/>o:
      <ekz>
        <tld/>a metodo.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Libera de mikroboj:
      <ekz>
        <tld/>a instrumento, vundo.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">aseptique</trd>
  <trd lng="ru">&c_a;&c_s;&c_je;&c_p;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="aseps.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Uzi <tld/>on.
    </dif>
  </snc>
  <trd lng="fr">aseptiser</trd>
</drv>
</art>
