<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>a&ubreve;ror</rad>/o
      <fnt>Z</fnt>
    </kap>

    <drv mrk="auxror.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Matena krepusko, matenru&gcirc;o:
          <ekz>
            <tld/>o kun rozkoloraj fingroj <klr>(konsiderata kiel manifesti&gcirc;o
              de la samnoma <ref cel="auxror.A0o">diino</ref>, beletra
              metaforo)</klr>.
          </ekz>
        </dif>
        <trdgrp lng="de">
          <trd>Morgenrot</trd>,
          <trd>Morgenr&ouml;te</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>aurore</trd>,
          <trd>lever du <ind>jour</ind></trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>hajnal</trd>,
          <trd>pirkadat</trd>
        </trdgrp>
        <trd
          lng="ru"><klr>(&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_ja;&c_ja;)</klr>
          &c_z;&c_a;&c_r;&c_ja;</trd>
      </snc>
      <snc>
        <uzo tip="stl">FIG</uzo>
        <dif>
          Esperiga komenci&gcirc;o:
          <ekz>
            &ccirc;e la <tld/>o de la vivo;
          </ekz><ekz>
            la <tld/>o de novaj tempoj.
          </ekz>
        </dif>
        <trdgrp lng="de">
          <trd>Zauber</trd>,
          <trd>Reiz des Neuen</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>aurore</trd>,
          <trd>aube</trd>
        </trdgrp>
        <trd lng="hu">hajnal</trd>
        <trd lng="ru">&c_z;&c_a;&c_r;&c_ja;</trd>
      </snc>
      <snc mrk="auxror.0o.AST">
        <uzo tip="fak">AST</uzo>
        <ref tip="dif" cel="lum.polusa0o">polusa lumo</ref>
        <refgrp tip="sub">
          <ref cel="auxror.arkta0o">arkta <tld/>o</ref>,
          <ref cel="auxror.antarkta0o">antarkta <tld/>o</ref>
        </refgrp>
        <rim>
          Estas nekomprenebla, kial oni tiel konfuze kaj misgvide nomu
          fenomenon, kiu nek ekstere nek fizike similas la komunsencan
          <tld/>on.
          <aut>Sergio Pokrovskij</aut>
        </rim>
        <trd lng="de">Polarlicht</trd>
        <trd lng="en">polar light</trd>
        <trd lng="fr"><ind>aurore</ind> polaire</trd>
        <trd lng="hu">sarki f&eacute;ny</trd>
        <trd lng="ru">&c_p;&c_o;&c_l;&c_ja;&c_r;&c_n;&c_o;&c_je;
&c_s;&c_i;&c_ja;&c_n;&c_i;&c_je;</trd>
      </snc>
      <trd lng="en">aurora</trd>
      <trd lng="es">aurora</trd>
    </drv>

    <drv mrk="auxror.A0o">
      <kap><tld lit="A"/>o, <tld lit="A"/>a</kap>
      <uzo tip="fak">MIT</uzo>
      <snc>
        <dif>
          Diino de la mateno en antikva mitologio.
        </dif>
        <trdgrp lng="de">
          <trd>Eos</trd>,
          <trd>Aurora</trd>
        </trdgrp>
        <trd lng="en">Aurora</trd>
        <trd lng="es">Aurora</trd>
        <trd lng="fr">Aurore</trd>
        <trd lng="hu">Aur&oacute;ra</trd>
        <trd lng="ru">&c_A;&c_v;&c_r;&c_o;&c_r;&c_a;</trd>
      </snc>
    </drv>

    <drv mrk="auxror.0i">
      <kap><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
        <gra><vspec>sensubjekte</vspec></gra>
        <dif>
          Esti la momento de <tld/>o.
        </dif>
        <trd lng="es">amanecer</trd>
      </snc>
      <snc>
        <dif>
          Eklumi, kiel <tld/>o:
          <ekz>
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            <tld/>as nova epoko.
          </ekz>
        </dif>
        <trdgrp lng="fr">
          <trd>poindre</trd>,
          <trd>percer</trd>,
          <trd>&eacute;merger</trd>
        </trdgrp>
      </snc>
      <trd lng="hu">hajnalodik</trd>
    </drv>

    <drv mrk="auxror.antarkta0o">
      <kap>antarkta <tld/>o</kap>
      <uzo tip="fak">AST</uzo>
      <snc>
        <dif>
          <ref tip="super" cel="auxror.0o.AST"><tld lit="A"/>o</ref>,
          okazanta &ccirc;e la suda poluso.
        </dif>
      </snc>
      <trd lng="fr"><ind>aurore</ind> australe</trd>
    </drv>

    <drv mrk="auxror.arkta0o">
      <kap>arkta <tld/>o</kap>
      <uzo tip="fak">AST</uzo>
      <snc>
        <ref cel="lum.nord0o" tip="dif">nordlumo</ref>
      </snc>
      <trd lng="fr"><ind>aurore</ind> bor&eacute;ale</trd>
    </drv>

    <drv mrk="auxror.polusa0o">
      <kap>polusa <tld/>o</kap>
      <uzo tip="fak">AST</uzo>
      <snc>
        <ref tip="dif" cel="auxror.0o.AST">a&ubreve;roro<sncref/></ref>
      </snc>
      <trd lng="fr"><ind>aurore</ind> polaire</trd>
    </drv>
  </art>
