<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>a&ubreve;ror</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="auxror.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Matena krepusko, matenru&gcirc;o:
      <ekz>
        <tld/>o kun rozkoloraj fingroj <klr>(konsiderata kiel manifesti&gcirc;o
        de la samnoma <ref cel="auxror.A0o">diino</ref>, beletra
        metaforo)</klr>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Morgenrot</trd>,
      <trd>Morgenr&ouml;te</trd>
    </trdgrp>
    <trd lng="es">aurora</trd>
    <trdgrp lng="fr">
      <trd>aurore</trd>,
      <trd>lever du <ind>jour</ind></trd>
    </trdgrp>
    <trd
    lng="ru"><klr>(&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_ja;&c_ja;)</klr>
    &c_z;&c_a;&c_r;&c_ja;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Esperiga komenci&gcirc;o:
      <ekz>
        &ccirc;e la <tld/>o de la vivo;
      </ekz>
      <ekz>
        la <tld/>o de novaj tempoj.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Zauber</trd>,
      <trd>Reiz des Neuen</trd>
    </trdgrp>
    <trd lng="es">aurora</trd>
    <trdgrp lng="fr">
      <trd>aurore</trd>,
      <trd>aube</trd>
    </trdgrp>
    <trd lng="ru">&c_z;&c_a;&c_r;&c_ja;</trd>
  </snc>
  <snc mrk="auxror.0o.AST">
    <uzo tip="fak">AST</uzo>
    <uzo tip="klr">(polusa <tld/>o)</uzo>
    <dif>
      Altatmosfera luma fenomeno, observebla &ccirc;e la magnetaj polusoj
      de la tero, estigita de sundevenaj elektrizitaj atomeroj:
      <ekz>
        <ref cel="arkt.0a">arkta</ref> <tld/>o
        (<ref cel="lum.nord0o" tip="sin">nordlumo</ref>);
      </ekz>
      <ekz>
        <ref cel="antark.0a">antarkta</ref> <tld/>o.
      </ekz>
    </dif>
    <trd lng="de">Polarlicht</trd>
    <trd lng="ru">&c_s;&c_je;&c_v;&c_je;&c_r;&c_n;&c_o;&c_je; &c_s;&c_i;&c_ja;&c_n;&c_i;&c_je;</trd>
  </snc>
  <trd lng="es">aurora</trd>
</drv>

<drv mrk="auxror.A0o">
  <kap><tld lit="A"/>o, <tld lit="A"/>a</kap>
  <uzo tip="fak">MIT</uzo>
  <snc>
    <dif>
      Diino de la mateno en antikva mitologio.
    </dif>
    <trdgrp lng="de">
      <trd>Eos</trd>,
      <trd>Aurora</trd>
    </trdgrp>
    <trd lng="es">Aurora</trd>
    <trd lng="fr">Aurore</trd>
    <trd lng="ru">&c_A;&c_v;&c_r;&c_o;&c_r;&c_a;</trd>
  </snc>
</drv>

<drv mrk="auxror.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <gra><vspec>sensubjekte</vspec></gra>
    <dif>
      Esti la momento de <tld/>o.
    </dif>
    <trd lng="es">amanecer</trd>
  </snc>
  <snc>
    <dif>
      Eklumi, kiel <tld/>o:
      <ekz>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>as nova epoko.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>poindre</trd>,
      <trd>percer</trd>,
      <trd>&eacute;merger</trd>
    </trdgrp>
  </snc>
</drv>
</art>
