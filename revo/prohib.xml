<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: prohib.xml,v 1.10 2018/11/20 17:10:20 revo Exp $">
<kap>
  <rad>prohib</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="prohib.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Oficiale <ref tip="dif" cel="permes.mal0i">malpermesi</ref>:
      <ekz>
        la <tld/>oj ne forigas ka&scirc;ajn, subtablajn traktadojn
        <fnt>
          <bib>Monato</bib>
          <lok><url ref="http://steloj.de/esperanto/monato2/007283.html"
              >http://steloj.de/esperanto/monato2/007283.html</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_b;&c_a;&c_r;&c_a;&c_n;&c_ja;&c_c;&c_mol;</trd>
  <trd lng="cs">zak&aacute;zat</trd>
  <trdgrp lng="de">
    <trd>verbieten</trd>,
    <trd>untersagen</trd>
  </trdgrp>
  <trd lng="es">prohibir</trd>
  <trd lng="fr">prohiber</trd>
  <trdgrp lng="hu">
    <trd>tilt</trd>,
    <trd>megtilt</trd>
  </trdgrp>
  <trd lng="nl">verbieden<klr tip="ind"> (ontr.)</klr></trd>
  <trd lng="pt">proibir</trd>
  <trdgrp lng="sk">
    <trd>nedovoli&tcaron;</trd>,
    <trd>zak&aacute;za&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">f&ouml;rbjuda</trd>
</drv>
</art>

