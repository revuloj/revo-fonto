<vortaro>
<art mrk="$Id$">
<kap>
  <rad>prohib</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="prohib.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <ref tip="dif" cel="permes.mal0i">malpermesi</ref>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_b;&c_a;&c_r;&c_a;&c_n;&c_ja;&c_c;&c_mol;</trd>
  <trd lng="es">prohibir</trd>
  <trd lng="fr">prohiber</trd>
  <trdgrp lng="hu">
    <trd>tilt</trd>,
    <trd>megtilt</trd>
  </trdgrp>
  <trd lng="pt">proibir</trd>
</drv>
</art>

