<drv mrk="kurb.0igxi">
  <kap>
    <tld/>i&gcirc;i, <var><kap><tld/>i</kap></var>
  </kap>
  <snc>
    <dif>
      Alpreni <tld/>an teni&gcirc;on:
      <ekz>
        lia bu&scirc;o <tld/>i&gcirc;is
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>i&gcirc;is super sia laboro
        <fnt>
          <bib>HsT</bib>
          <lok>Heredanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>is pro malvarmo en mia korto
        <fnt>
          <bib>MkM</bib>
          <lok>13</lok>
        </fnt>;
      </ekz>
      <ekz>
        al la kafej&#8217; eniras oni rekte, senbalance, sed elvenante oni <klr>[&#8230;]</klr>
        <tld/>as &ccirc;e la placorando, al stratkanalo fluas multa brando
        <fnt>
          <bib>VRA</bib>
          <lok>Jan Frideg&aring;rd</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;i anta&ubreve; iu
        (<refgrp tip="vid">
          <ref cel="rivere.0i">riverenci</ref>,
          <ref cel="klin.0igxi">klini&gcirc;i</ref>
        </refgrp>);
      </ekz>
      <ekz>
        <tld/>i&gcirc;u hoko la&ubreve; postuloj de l' loko
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;adi <klr>(skui&gcirc;i, barakti)</klr>
        kiel diablo en akvo benita
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_y;&c_v;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_y;&c_w;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_g;&c_ib;&c_n;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sich <ind>kr&uuml;mmen</ind></trd>,
    <trd>sich <ind>beugen</ind></trd>
  </trdgrp>
  <trd lng="en">curve</trd>
  <trd lng="fr">se <ind>courber</ind></trd>
</drv>

