<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kurb</rad>/a
</kap>
<drv mrk="kurb.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Kvalifikas linion a&ubreve; objekton, kiu estas nek rekta,
      nek konsistanta el rektaj partoj:
      <ekz>
        <tld/>a bastono;
      </ekz>
      <ekz>
        <tld/>a bran&ccirc;o;
      </ekz>
      <ekz>
        la supra&jcirc;o de la tero estas <tld/>a;
      </ekz>
      <ekz>
        <tld/>eco de arko, de sfero, de volbo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rond.0a">ronda</ref>,
      <ref cel="oval.0a">ovala</ref>,
      <ref cel="form">arkforma</ref>,
      <ref cel="serpen.0a">serpenta</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>kurvig</trd>,
     <trd>kurvenf&ouml;rmig</trd>,
     <trd>gekr&uuml;mmt</trd>
  </trdgrp>  <trdgrp lng="en">
    <trd>curved</trd>,
    <trd>bandy</trd>,
    <trd>bow(ed)</trd>,
    <trd>tortuous</trd>,
    <trd>winding</trd>
  </trdgrp>
  <trd lng="fr">courbe<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="nl">krom</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_i;&c_v;&c_o;&c_j;</trd>,
    <trd>&c_i;&c_z;&c_o;&c_g;&c_n;&c_u;&c_t;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>krokig</trd>,
    <trd>sv&auml;ngd</trd>,
    <trd>kurvig</trd>
  </trdgrp>
</drv>

<drv mrk="kurb.0o">
  <kap><tld/>o</kap>
  <snc mrk="kurb.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 31</lok></fnt>
    <dif>
      <ref tip="dif" cel="lini.0o.MAT">Linio</ref>, precipe kiam
      ne temas pri rekto:
      <ekz>
        <ind>ebena <tld/>o</ind> <klr>(inkluzivata en ebeno)</klr>;
        <trd lng="de">ebene <ind>Kurve</ind></trd>
        <trd lng="en">plane <ind>curve</ind></trd>
        <trd lng="fr">courbe plane</trd>
        <trd lng="ru">&c_p;&c_l;&c_o;&c_s;&c_k;&c_a;&c_ja; <ind>&c_k;&c_r;&c_i;&c_v;&c_a;&c_ja;</ind></trd>
      </ekz>
      <ekz>
        <ind>neebena <tld/>o</ind>;
        <trd lng="de">Raumkurve</trd>
        <trd lng="en">space <ind>curve</ind></trd>
        <trd lng="fr">courbe gauche</trd>
        <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_a;&c_ja; <ind>&c_k;&c_r;&c_i;&c_v;&c_a;&c_ja;</ind></trd>
      </ekz>
      <ekz>
        polusa, kartezia <ref cel="ekvaci.0o">ekvacio</ref> de <tld/>o;
      </ekz>
      <ekz>
        algebra, transcenda <tld/>o <klr>(kies kartezia ekvacio estas
        algebra, transcenda)</klr>;
      </ekz>
      <ekz>
        n-a-grada ebena <tld/>o <klr>(kies kartezia ekvacio esprimi&gcirc;as
        per n-a-grada polinoma funkcio de la koordinatoj)</klr>.
      </ekz>
    </dif>
    <ref tip="super" cel="figur.geometria0o">geometria figuro;</ref>
    <refgrp tip="sub">
      Specifaj ebenaj kurboj:
      <ref cel="spiral.arhximeda0o">ar&hcirc;imeda spiralo</ref>,
      <ref cel="cikloi.0o">cikloido</ref>,
      <ref cel="cirkl.0o.linio">cirklo</ref>,
      <ref cel="cisoid.0o">cisoido</ref>,
      <ref cel="elips.0o.linio">elipso</ref>,
      <ref cel="epicik.0o">epicikloido</ref>,
      <ref cel="hiperb.0o.linio">hiperbolo</ref>,
      <ref cel="hipoci.0o">hipocikloido</ref>,
      <ref cel="kardio.0o">kardioido</ref>,
      <ref cel="oval.kartezia0o">kartezia ovalo</ref>,
      <ref cel="katen.0o.linio">kateno</ref>,
      <ref cel="konik.0o">koniko</ref>,
      <ref cel="lemnis.0o">lemniskato</ref>,
      <ref cel="spiral.logaritma0o">logaritma spiralo</ref>,
      <ref cel="parabo.0o.linio">parabolo</ref>,
      <ref cel="limak.paskala0o">paskala limako</ref>,
      <ref cel="strofo.0o">strofoido</ref>,
      <ref cel="trakto1.0o">traktorio</ref>;
    </refgrp>
    <refgrp tip="sub">
      Specifa neebena kurbo:
      <ref cel="helic.0o.MAT">helico</ref>;
    </refgrp>
    <refgrp tip="vid">
      Karakteriza&jcirc;oj de ebena kurbo:
      <ref cel="kurb.0eco.de_ebena_kurbo">kurbeco</ref>,
      <ref cel="centr.kurbeco0o.de_ebena_kurbo">kurbecocentro</ref>,
      <ref cel="cirkl.kurbeco0o.de_ebena_kurbo">kurbecocirklo</ref>,
      <ref cel="radius.kurbeco0o.de_ebena_kurbo">kurbecoradiuso</ref>;
    </refgrp>
    <refgrp tip="sub">
      Derivitaj kurboj:
      <ref cel="volv.el0anto">elvolvanto</ref>,
      <ref cel="volv.el0ato">elvolvato</ref>,
      <ref cel="volv.en0ajxo.de_kurboj">envolva&jcirc;o</ref>,
      <ref cel="konhxo.0o">kon&hcirc;oido</ref>,
      <ref cel="podajr.0o">podajro</ref>.
    </refgrp>
    <rim>
      <tld lit="K"/>ojn oni studas &ccirc;efe surbaze de
      la analitikaj ecoj de iliaj ekvacioj a&ubreve; de iliaj parametraj
      prezentoj.
    </rim>
    <trd lng="de">Kurve</trd>
    <trd lng="en">curve</trd>
  </snc>
  <snc mrk="kurb.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="super" cel="grafik.0ajxo">Grafika&jcirc;o</ref>,
      uzata por prezenti la evoluon de iu fenomeno:
      <ekz>
        klo&scirc;forma <tld/>o;
      </ekz>
      <ekz>
        <tld/>o de elspezoj de entrepreno la&ubreve; la jaroj.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Kurve</trd>,
      <trd>Graph</trd>,
      <trd>Grafik</trd>,
      <trd>Linie</trd>
    </trdgrp>  </snc>
  <trd lng="fr">courbe<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="ru">&c_k;&c_r;&c_i;&c_v;&c_a;&c_ja;</trd>
  <trd lng="sv">kurva</trd>
</drv>

<drv mrk="kurb.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="kurb.0eco.KOMUNE">
    <dif>
      Eco de io <tld/>a:
      <ekz>
        la <tld/>eco de la tero <klr>(la fakto, ke &gcirc;i ne
        estas ebena)</klr>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="kurb.0eco.de_ebena_kurbo">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 38</lok></fnt>
    <dif>
      <klr>(de ebena <ref cel="kurb.0o.MAT">kurbo</ref>, &ccirc;e punkto 
      <frm><k>a</k></frm> de &gcirc;i)</klr>
      La <ref cel="invers.0o.neuxtriganto">inverso</ref> de &gcirc;ia
      <ref cel="radius.kurbeco0o.de_ebena_kurbo"><tld/>ecoradiuso</ref>:
      <ekz>
        la <tld/>eco de <ref cel="rekt.0o.MAT">rekto</ref> estas nula.
      </ekz>
    </dif>
    <trd lng="de">Kr&uuml;mmung</trd>
    <trd lng="en">curvature</trd>
    <trd lng="fr">courbure</trd>
    <trd lng="ru">&c_k;&c_r;&c_i;&c_v;&c_i;&c_z;&c_n;&c_a;</trd>
  </snc>
</drv>

<drv mrk="kurb.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari ion momente a&ubreve; definitive <tld/>a:
      <ekz>
        la vento <tld/>igas la kanojn;
      </ekz>
      <ekz>
        <tld/>igi balena&jcirc;on, bran&ccirc;on, &scirc;tallamenon;
      </ekz>
      <ekz>
        <tld/>igi per fajro ferstangon;
      </ekz>
      <ekz>
        la a&gcirc;o <tld/>igas homon;
      </ekz>
      <ekz>
        la arboj <tld/>i&gcirc;is sub la fruktoj;
      </ekz>
      <ekz>
        <tld/>i&gcirc;i anta&ubreve; iu
        (<refgrp tip="vid">
          <ref cel="rivere.0i">riverenci</ref>,
          <ref cel="klin.0igxi">klini&gcirc;i</ref>
        </refgrp>);
      </ekz>
      <ekz>
        ne <tld/>i&gcirc;i sub la jugo de despotismo;
      </ekz>
      <ekz>
        <tld/>i&gcirc;adi <klr>(skui&gcirc;i, barakti)</klr>
        kiel diablo en akvo benita
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="fleks.0i">fleksi.</ref>
  </snc>
  <trdgrp lng="de">
      <trd>biegen</trd>,
      <trd>beugen</trd>,
      <trd>kr&uuml;mmen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>curve</trd>,
    <trd>arch</trd>,
    <trd>bend</trd>,
    <trd>turn</trd>,
    <trd>warp</trd>
  </trdgrp>
  <trd lng="fr">courber</trd>
  <trd lng="nl">krommen</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_k;&c_r;&c_i;&c_v;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_z;&c_o;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>kr&ouml;ka</trd>,
    <trd>b&ouml;ja</trd>
  </trdgrp>
</drv>

<drv mrk="kurb.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc>
    <dif>
      <tld lit="K"/>a parto de io:
      <ekz>
        <tld/>i&gcirc;o de vojo, de rivero;
      </ekz>
      <ekz>
        la vojeto grimpis per multaj <tld/>i&gcirc;oj &gcirc;is la supro de 
        la monto.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="turn">turni&gcirc;o</ref>,
      <ref cel="ond">ondolinio</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
      <trd>Kurve</trd>,
      <trd>Biege</trd>,
      <trd>Biegung</trd>,
      <trd>Beugung</trd>,
      <trd>Kr&uuml;mmung</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>curve</trd>,
    <trd>crook</trd>,
    <trd>bend</trd>,
    <trd>turn</trd>
  </trdgrp>
  <trd lng="nl">kromming</trd>
  <trd lng="fr">courbure<klr tip="ind"> (partie courbe)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_k;&c_r;&c_i;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_i;&c_z;&c_g;&c_i;&c_b;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>kurva</trd>,
    <trd>kr&ouml;k</trd>,
    <trd>sv&auml;ng</trd>
  </trdgrp>
</drv>
</art>
