<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxpros.xml,v 1.6 2024/11/26 08:36:16 revo Exp $">
<kap>
  <rad>&scirc;pros</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sxpros.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxpros.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Vergo, parto de <ref tip="vid" cel="fram.0o.TEK">framo</ref>,
      kiu dividas la vitropecojn de fenestro:
      <ekz>
        ornama <tld/>o
        <fnt>
          <vrk><url ref="http://take.esperanto.free.fr/KK-2020/kk-2020.pdf"
              >Fenestroj</url></vrk>
          <lok>Konstruado (TAKE) 2020, n-ro 5, p. 12, bildo 3: Vertikala sekco</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Ailin</nom> aliris al la fenestro kaj apogante sian kapon al &gcirc;ia
        <tld/>o, direktis sian rigardon al la pluva nebulo ekstere
        <fnt>
          <aut>Qiong Yao, trad. Vejdo</aut>,
          <vrk><url ref="http://www.ipernity.com/group/115874/discuss/130589">
          En profunda korto (28)</url></vrk>,
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fost.0o">fosto</ref>,
      <ref cel="rung.0o">rungo</ref>,
      <ref cel="steg.0o">stego</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_p;&c_ja;&c_r;&c_o;&c_ch;&c_y;&c_n;&c_a;</trd>,
    <trd>&c_ib;&c_m;&c_p;&c_o;&c_s;&c_t; <klr>(&c_a;&c_k;&c_n;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="bo">&t_ra;&t_gata;&t_yata;&t_sa;&t_tsheg;</trd>
  <trdgrp lng="de">
    <trd>Sprosse <klr>(Fenster)</klr></trd>,
    <trd>Pfosten <klr>(Fehsterrahmen)</klr></trd>,
    <trd>Setzholz</trd>,
    <trd>Riegel <klr>(Fehsterrahmen)</klr></trd>
  </trdgrp>
  <trd lng="fr">meneau</trd>
  <trd lng="pl">szpros</trd>
  <trdgrp lng="uk">
    <trd>&c_o;&c_b;&c_a;&c_p;&c_o;&c_l;&c_o;&c_k; &c_v;&c_ib;&c_k;&c_o;&c_n;&c_n;&c_o;&c_ji;
    &c_r;&c_a;&c_m;&c_i;</trd>,
    <trd>&c_p;&c_o;&c_p;&c_je;&c_r;&c_je;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_p;&c_je;&c_r;&c_je;&c_ch;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
</drv>
</art>

