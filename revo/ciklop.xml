<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ciklop.xml,v 1.29 2023/10/31 13:37:03 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>ciklop</rad>/o <fnt><bib>Hamlet</bib></fnt>
</kap>
<drv mrk="ciklop.0o">
  <kap><tld/>o</kap>
  <snc mrk="ciklop.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Mitologia giganta for&gcirc;isto, kun unu sola okulo
      en la frunto:
      <ekz>
        neniam la marteloj de l&#x2019; ciklopoj, / por Marso elfor&gcirc;ante la arma&jcirc;on, / batadis
        pli kruele kaj sova&gcirc;e, / ol kiel nun la sanga glav&#x2019; de Pirro
        <fnt>
          <bib>Hamlet</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_k;&c_l;&c_jo;&c_p;</trd>
  <trd lng="ca">ciclop</trd>
  <trdgrp lng="cs">
    <trd>Kyklop</trd>,
    <trd>kyklop</trd>
  </trdgrp>
  <trd lng="de">Zyklop</trd>
  <trd lng="el">&kappa;&ypsilon_ton;&kappa;&lambda;&omega;&pi;&alfa;&sigma_fina;</trd>
  <trd lng="en">cyclops</trd>
  <trd lng="es">c&iacute;clope</trd>
  <trd lng="fr">cyclope</trd>
  <trd lng="he">&#x5E6;&#x5D9;&#x5E7;&#x5DC;&#x5D5;&#x5E4;</trd>
  <trdgrp lng="hu">
    <trd>k&uuml;kloposz</trd>,
    <trd>ciklopsz</trd>
  </trdgrp>
  <trd lng="it">ciclope</trd>
  <trd lng="ja">&#x30AD;&#x30E5;&#x30AF;&#x30ED;&#x30D7;&#x30B9;</trd>
  <trd lng="pl">cyklop</trd>
  <trd lng="pt">ciclope</trd>
  <trd lng="ru">&c_c;&c_i;&c_k;&c_l;&c_o;&c_p;</trd>
  <trdgrp lng="sk">
    <trd>Kyklop</trd>,
    <trd>kyklop</trd>
  </trdgrp>
  <trd lng="uk">&#x446;&#x438;&#x43A;&#x43B;&#x43E;&#x43F;</trd>
  <trd lng="zh">&#x72EC;&#x773C;&#x5DE8;&#x4EBA; <pr>d&#xFA;y&#x1CE;nj&#xF9;r&#xE9;n</pr></trd>
</drv>

<drv mrk="ciklop.0a">
  <kap><tld/>a</kap>
  <snc mrk="ciklop.0a.MIT">
    <dif>
      De <tld/>o.
    </dif>
    <trd lng="de">zyklopisch</trd>
    <trd lng="hu">k&uuml;klopszi</trd>
    <trd lng="zh">&#x4E71;&#x77F3;&#x5806; <pr>lu&#xE0;nsh&#xED;du&#x12B;</pr></trd>
</snc>
  <snc mrk="ciklop.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kvaza&ubreve; farita de grandeguloj,
      <ref tip="vid" cel="gigant.0a">giganta</ref>,
      <ref tip="vid" cel="kolos.0a">kolosa</ref>,
      <ref tip="vid" cel="titan.0a">titana</ref>.
    </dif>
    <trdgrp lng="de">
    <trd>kolossal</trd>,
    <trd>gigantisch</trd></trdgrp>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_k;&c_l;&c_ja;&c_p;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">ciclopi</trd>
  <trd lng="cs">obrovsk&yacute;</trd>
  <trd lng="el">&kappa;&ypsilon;&kappa;&lambda;&omega_ton;&pi;&epsilon;&jota;&omikron;&sigma_fina;</trd>
  <trd lng="en">cyclopean</trd>
  <trd lng="es">cicl&oacute;peo</trd>
  <trd lng="fr">cyclop&eacute;en</trd>
  <trd lng="he">&#x5E6;&#x5D9;&#x5E7;&#x5DC;&#x5D5;&#x5E4;&#x5D9;</trd>
  <trd lng="hu">gig&aacute;szi</trd>
  <trd lng="it">ciclopico</trd>
  <trdgrp lng="ja">
    <trd>&#x30AD;&#x30E5;&#x30AF;&#x30ED;&#x30D7;&#x30B9;&#x306E;</trd>,
    <trd>&#x5DE8;&#x5927;&#x306A;  <pr>&#x304D;&#x3087;&#x3060;&#x3044;&#x306A;</pr></trd>
  </trdgrp>
  <trd lng="pl">cyklopowy</trd>
  <trd lng="pt">cicl&oacute;pico</trd>
  <trd lng="ru">&c_c;&c_i;&c_k;&c_l;&c_o;&c_p;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>kyklopsk&yacute;</trd>,
    <trd>obrovsk&yacute;</trd>
  </trdgrp>
</drv>
</art>
