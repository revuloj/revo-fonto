<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
    <kap>
        <ofc>2</ofc>
        <rad>ciklop</rad>/o
    </kap>
    <drv mrk="ciklop.0o">
        <kap><tld/>o</kap>
        <uzo tip="fak">MIT</uzo>
        <snc>
            <dif>
            Mitologia giganta for&gcirc;isto, kun unu sola okulo en la
            frunto.
            </dif>
        </snc>
        <trd lng="be">&c_c;&c_y;&c_k;&c_l;&c_jo;&c_p;</trd>
        <trd lng="de">Zyklop</trd>
        <trd lng="el">&kappa;&ypsilon_ton;&kappa;&lambda;&omega;&pi;&alfa;&sigma_fina;</trd>
        <trd lng="es">c&iacute;clope</trd>
        <trd lng="fr">cyclope</trd>
        <trdgrp lng="hu">
        <trd>k&uuml;kloposz</trd>,
        <trd>ciklopsz</trd></trdgrp>
        <trd lng="pl">cyklop</trd>
        <trd lng="pt">ciclope</trd>
        <trd lng="ru">&c_c;&c_i;&c_k;&c_l;&c_o;&c_p;</trd>
        <trd lng="sk">Kyklop</trd>
    </drv>

    <drv mrk="ciklop.0a">
        <kap><tld/>a</kap>
        <snc num="1">
            <dif>
            De <tld/>o.
            </dif>
            <trd lng="de">zyklopisch</trd>
            <trd lng="hu">k&uuml;klopszi</trd>
        </snc>
        <snc num="2">
            <uzo tip="stl">FIG</uzo>
            <dif>
            Kolosa.
            </dif>
            <trdgrp lng="de">
            <trd>kolossal</trd>,
            <trd>gigantisch</trd></trdgrp>
        </snc>
        <trd lng="be">
        &c_c;&c_y;&c_k;&c_l;&c_ja;&c_p;&c_ib;&c_ch;&c_n;&c_y;</trd>
        <trd lng="el">&kappa;&ypsilon;&kappa;&lambda;&omega_ton;&pi;&epsilon;&jota;&omikron;&sigma_fina;</trd>
        <trd lng="fr">cyclop&eacute;en</trd>
        <trd lng="hu">gig&aacute;szi</trd>
        <trd lng="pl">cyklopowy</trd>
        <trd lng="pt">cicl&oacute;pico</trd>
        <trd lng="ru">&c_c;&c_i;&c_k;&c_l;&c_o;&c_p;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
    </drv>
</art>
