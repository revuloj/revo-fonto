<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>geopolitik</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="geopolitik.0o">
  <kap><tld/>o</kap>
  <snc mrk="geopolitik.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="politi.0o.POL">Politiko</ref> enkalkulanta
      geografiajn kondi&ccirc;ojn naturajn kaj homajn, kaj teritorian 
      konkuradon inter regnoj:
      <ekz>
        en Grandbritujo li studis <tld/>on kaj inter&scirc;atajn rilatojn
        <fnt>
          <aut>Konsilio pri aferoj de Litovio Minora</aut>,
          <vrk><url ref="http://www.mlrt.puslapiai.lt/esp/regis.doc">
          Honora membro: Algis Regis...</url></vrk>,
          <lok>2004-04</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">g&eacute;opolitique<klr> (subst.)</klr></trd>
  <trd lng="pt">geopol&#237;tica</trd>
</drv>

<drv mrk="geopolitik.0a">
  <kap><tld/>a</kap>
  <snc mrk="geopolitik.0a.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a graveco de korea duoninsulo
        <fnt>
          <aut>Dekmil</aut>,
          <vrk>Manenmane por la Unui&gcirc;o</vrk>,
          <lok><url ref="http://solidareco.cafe24.com/17.php">
          La Solidareco, 2005-09, numero 17a</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">g&eacute;opolitique<klr> (adj.)</klr></trd>
</drv>
</art>

