<drv mrk="vici.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Vicia</trd>). Kelkaj specioj estas kulturataj,
      ekzemple la <ref tip="sub" cel="fab.0o">fabo</ref>:
      <ekz>
        <ind>Birda vicio</ind>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=21277">Birda vicio</url></lok>
        </fnt>.
        <trd lng="de">Vogelwicke</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_k;&c_a;</trd>
  <trd lng="ca">g&egrave;nere Vicia</trd>
  <trd lng="cs">vikev</trd>
  <trd lng="de">Wicke</trd>
  <trd lng="en">vetch</trd>
  <trd lng="fr">vesce</trd>
  <trd lng="hu">b&uuml;kk&ouml;ny</trd>
  <trd lng="ja">&#x30BD;&#x30E9;&#x30DE;&#x30E1;</trd>
  <trd lng="pl">wyka</trd>
  <trd lng="nl">wikke</trd>
  <trd lng="ru">&c_v;&c_i;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>vika</trd>,
    <trd>&lcaron;adn&iacute;k k&racute;mny</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&#x43A;&c_a;</trd>,
    <trd>&c_g;&#x43E;&c_r;&#x43E;&c_sh;&#x43E;&#x43A;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#34183; <pr>w&#xE9;i</pr></trd>,
    <trd>&#x91CE;&#x8C4C;&#x8C46; <pr>y&#x11B;w&amacron;nd&#xF2;u</pr></trd>
  </trdgrp>
</drv>
