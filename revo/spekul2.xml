<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: spekul2.xml,v 1.19 2023/10/21 07:46:48 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>spekulativ</rad>/a
</kap>
<drv mrk="spekul2.0a">
  <kap><tld/>a</kap>
  <snc mrk="spekul2.0a.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Bazita sur pure teoria rezonado, celante nur la scion, sen ia ajn
      praktika utilo:
      <ekz>
        tio havas nur <tld/>an intereson.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kabine">kabineta</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_e;&c_k;&c_u;&c_l;&c_ja;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>
  <trdgrp lng="bg">
    <trd>&c_s;&c_p;&c_je;&c_k;&c_u;&c_l;&c_a;&c_t;&c_i;&c_v;&c_je;&c_n;</trd>,
    <trd>&c_u;&c_m;&c_o;&c_z;&c_r;&c_i;&c_t;&c_je;&c_l;&c_je;&c_n;</trd>
  </trdgrp>
  <trd lng="ca">especulatiu</trd>
  <trd lng="de">spekulativ</trd>
  <trd lng="en">speculative</trd>
  <trd lng="es">especulativo</trd>
  <trd lng="fr">sp&eacute;culatif</trd>
  <trdgrp lng="hu">
    <trd>spekulat&iacute;v</trd>,
    <trd>elm&eacute;leti</trd>,
    <trd>elvont</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x601D;&#x5F01;&#30340;&#x306A;  <pr>&#x304A;&#12418;&#12409;&#12435;&#12390;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x7D14;&#29702;&#x8AD6;&#30340;&#x306A;  <pr>&#12376;&#12421;&#12435;&#x308A;&#x308D;&#12435;&#12390;&#x304D;&#x306A;</pr></trd>
  </trdgrp>
  <trd lng="nl">speculatief</trd>
  <trd lng="pl">spekulatywny</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_je;&c_k;&c_u;&c_l;&c_ja;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_u;&c_m;&c_o;&c_z;&c_r;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43F;&c_je;&#x43A;&c_u;&#x43B;&#x44F;&c_t;&c_i;&c_v;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_u;&#x43C;&#x43E;&c_g;&#x43B;&#x44F;&c_d;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="spekul2.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="spekul2.0i.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      <tld lit="S"/>e koncepti:
      <ekz>
        pri tio la filozofoj povas nur <tld/>i per hipotezoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_e;&c_k;&c_u;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_r;&c_a;&c_z;&c_v;&c_a;&c_zh;&c_a;&c_c;&c_mol;)</klr></trd>
  <trd lng="bg">&c_r;&c_a;&c_z;&c_m;&c_i;&c_sh;&c_l;&c_ja;&c_v;&c_a;&c_m;</trd>
  <trd lng="en">speculate</trd>
  <trd lng="es">especular</trd>
  <trd lng="fr">sp&eacute;culer</trd>
  <trdgrp lng="hu">
    <trd>spekul&aacute;l</trd>,
    <trd>elm&eacute;lkedik</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6C88;&#x601D;&#12377;&#x308B;  <pr>&#12375;&#x305A;&#x304A;&#12418;&#12377;&#x308B;</pr></trd>,
    <trd>&#x601D;&#x7D22;&#12377;&#x308B;  <pr>&#12375;&#12373;&#x304F;&#12377;&#x308B;</pr></trd>,
    <trd>&#x61B6;&#x6E2C;&#12377;&#x308B;  <pr>&#x304A;&#x304F;&#x305D;&#x304F;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">bespiegelingen houden</trd>
  <trd lng="pl">spekulowa&cacute; umys&lstroke;owo</trd>
  <trd lng="uk">&c_r;&#x43E;&c_b;&c_i;&c_t;&c_i; &#x43F;&c_r;&c_i;&#x43F;&c_u;&c_shch;&c_je;&#x43D;&#x43D;&#x44F;</trd>
</drv>
</art>
