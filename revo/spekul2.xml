<art mrk="$Id: spekul2.xml,v 1.17 2023/09/04 05:14:57 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>spekulativ</rad>/a
</kap>
<drv mrk="spekul2.0a">
  <kap><tld/>a</kap>
  <snc mrk="spekul2.0a.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Bazita sur pure teoria rezonado, celante nur la scion, sen ia ajn
      praktika utilo:
      <ekz>
        tio havas nur <tld/>an intereson.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kabine">kabineta</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_e;&c_k;&c_u;&c_l;&c_ja;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>
  <trdgrp lng="bg">
    <trd>&c_s;&c_p;&c_je;&c_k;&c_u;&c_l;&c_a;&c_t;&c_i;&c_v;&c_je;&c_n;</trd>,
    <trd>&c_u;&c_m;&c_o;&c_z;&c_r;&c_i;&c_t;&c_je;&c_l;&c_je;&c_n;</trd>
  </trdgrp>
  <trd lng="ca">especulatiu</trd>
  <trd lng="de">spekulativ</trd>
  <trd lng="en">speculative</trd>
  <trd lng="es">especulativo</trd>
  <trd lng="fr">sp&eacute;culatif</trd>
  <trdgrp lng="hu">
    <trd>spekulat&iacute;v</trd>,
    <trd>elm&eacute;leti</trd>,
    <trd>elvont</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x601D;&#x5F01;&#x7684;&#x306A;  [<ind>&#x304A;&#x3082;&#x3079;&#x3093;&#x3066;&#x304D;&#x306A;</ind>]</trd>,
    <trd>&#x7D14;&#x7406;&#x8AD6;&#x7684;&#x306A;  [<ind>&#x3058;&#x3085;&#x3093;&#x308A;&#x308D;&#x3093;&#x3066;&#x304D;&#x306A;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">speculatief</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_je;&c_k;&c_u;&c_l;&c_ja;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_u;&c_m;&c_o;&c_z;&c_r;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x441;&#x43F;&#x435;&#x43A;&#x443;&#x43B;&#x44F;&#x442;&#x438;&#x432;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x443;&#x43C;&#x43E;&#x433;&#x43B;&#x44F;&#x434;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="spekul2.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="spekul2.0i.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      <tld lit="S"/>e koncepti:
      <ekz>
        pri tio la filozofoj povas nur <tld/>i per hipotezoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_e;&c_k;&c_u;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_r;&c_a;&c_z;&c_v;&c_a;&c_zh;&c_a;&c_c;&c_mol;)</klr></trd>
  <trd lng="bg">&c_r;&c_a;&c_z;&c_m;&c_i;&c_sh;&c_l;&c_ja;&c_v;&c_a;&c_m;</trd>
  <trd lng="en">speculate</trd>
  <trd lng="es">especular</trd>
  <trd lng="fr">sp&eacute;culer</trd>
  <trdgrp lng="hu">
    <trd>spekul&aacute;l</trd>,
    <trd>elm&eacute;lkedik</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6C88;&#x601D;&#x3059;&#x308B;  [<ind>&#x3057;&#x305A;&#x304A;&#x3082;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x601D;&#x7D22;&#x3059;&#x308B;  [<ind>&#x3057;&#x3055;&#x304F;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x61B6;&#x6E2C;&#x3059;&#x308B;  [<ind>&#x304A;&#x304F;&#x305D;&#x304F;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">bespiegelingen houden</trd>
  <trd lng="uk">&#x440;&#x43E;&#x431;&#x438;&#x442;&#x438; &#x43F;&#x440;&#x438;&#x43F;&#x443;&#x449;&#x435;&#x43D;&#x43D;&#x44F;</trd>
</drv>
</art>
