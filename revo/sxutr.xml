<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxutr.xml,v 1.13 2023/12/14 07:24:51 revo Exp $">
<kap>
  <rad>&scirc;utr</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="sxutr.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxutr.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Solida fenestro&scirc;irmilo, ekzemple el moveblaj tabuloj
      a&ubreve;
      el ruleblaj latoj:
      <ekz>
        la vento fajfas kaj &scirc;ancelas <tld/>ojn
        <fnt><bib>Mary</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_a;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="de">Fensterladen</trd>
  <trd lng="en">window shutter</trd>
  <trd lng="fr">volet</trd>
  <trdgrp lng="hu">
    <trd>spaletta</trd>,
    <trd>ablakt&aacute;bla</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>imposta<klr> (serramento)</klr></trd>,
    <trd>persiana</trd>,
    <trd>scuro</trd>,
    <trd>tapparella</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30B7;&#x30E3;&#x30C3;&#x30BF;&#x30FC;</trd>,
    <trd>&#12424;&#x308D;&#12356;&#25144;  <pr>&#12424;&#x308D;&#12356;&#12393;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>blind<klr tip="ind"> (luik)</klr></trd>,
    <trd>luik</trd>
  </trdgrp>
  <trd lng="pl">okiennica</trd>
  <trdgrp lng="pt">
    <trd>porta <klr>de janela</klr></trd>,
    <trd>contravento</trd>,
    <trd>guarda-vento</trd>
   </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&#x43A;&#x43E;&#x43D;&#x43D;&c_i;&c_c;&#x44F;</trd>,
    <trd>&c_zh;&c_a;&#x43B;&#x44E;&c_z;&c_ib;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_v;&#x43E;&c_r; (&c_u; &c_f;&#x43E;&c_t;&#x43E;&c_a;&#x43F;&c_a;&c_r;&c_a;&c_t;&c_ib; &c_ib; &c_t;.&#x43F;.)</trd>
  </trdgrp>
</drv>

<drv mrk="sxutr.rul0o">
  <kap>rul<tld/>o</kap>
  <snc>
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Fenestro&scirc;irmilo el ruleblaj (plastaj, metalaj a&ubreve;
      lignaj) latoj.
   </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_zh;&c_a;&c_l;&c_ju;&c_z;&c_ib;</trd>,
     <trd>&c_r;&c_a;&c_l;&c_mol;&c_sh;&c_t;&c_o;&c_r;&c_a;</trd>
   </trdgrp>
   <trd lng="fr">volet roulant</trd>
   <trd lng="it">tapparella</trd>
   <trd lng="nl">rolluik</trd>
   <trd lng="pl">okiennica</trd>
</drv>
</art>

