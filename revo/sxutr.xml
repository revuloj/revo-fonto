<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxutr.xml,v 1.16 2025/04/06 06:47:43 revo Exp $">
<kap>
  <rad>&scirc;utr</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="sxutr.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxutr.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Solida fenestro&scirc;irmilo, ekzemple el moveblaj tabuloj
      a&ubreve; el ruleblaj latoj; <ref tip="sin" cel="kovr.fenestro0ilo">fenestrokovrilo</ref>:
      <ekz>
        la vento fajfas kaj &scirc;ancelas <tld/>ojn
        <fnt><bib>Mary</bib></fnt>;
      </ekz>
      <ekz>
        mateno grizigas la &ccirc;ambron, li kutimis fermi <tld/>ojn
        <fnt>
          <bib>OIP</bib>
          <lok>Ombro sur interna pejza&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        fremda lumo <klr>[&#8230;]</klr> falas oblikve sur la liton, nek persieno nek <tld/>o
        troveblas
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2006/008760.php">En fremda lando</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Anka&#365; eblas diri <ke>fenestrokovrilo</ke>, <ke>fenestro&#349;irmilo</ke>.
  </rim>

  <trd lng="be">&c_a;&c_k;&c_a;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="de">Fensterladen</trd>
  <trd lng="en">window shutter</trd>
  <trd lng="fr">volet</trd>
  <trdgrp lng="hu">
    <trd>spaletta</trd>,
    <trd>ablakt&aacute;bla</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>imposta<klr> (serramento)</klr></trd>,
    <trd>persiana</trd>,
    <trd>scuro</trd>,
    <trd>tapparella</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12471;&#12515;&#12483;&#12479;&#12540;</trd>,
    <trd>&#12424;&#12429;&#12356;&#25144;  <pr>&#12424;&#12429;&#12356;&#12393;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>blind<klr tip="ind"> (luik)</klr></trd>,
    <trd>luik</trd>
  </trdgrp>
  <trd lng="pl">okiennica</trd>
  <trdgrp lng="pt">
    <trd>porta <klr>de janela</klr></trd>,
    <trd>contravento</trd>,
    <trd>guarda-vento</trd>
   </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_k;&c_o;&c_n;&c_n;&c_i;&c_c;&c_ja;</trd>,
    <trd>&c_zh;&c_a;&c_l;&c_ju;&c_z;&c_ib;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_v;&c_o;&c_r; (&c_u; &c_f;&c_o;&c_t;&c_o;&c_a;&c_p;&c_a;&c_r;&c_a;&c_t;&c_ib; &c_ib; &c_t;.&c_p;.)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#30334;&#21494;&#31383; <pr>b&#462;iy&egrave;chu&amacron;ng</pr></trd>,
    <trd>&#30334;&#33865;&#31383; <pr>b&#462;iy&egrave;chu&amacron;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxutr.rul0o">
  <kap>rul<tld/>o</kap>
  <snc>
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Fenestro&scirc;irmilo el ruleblaj (plastaj, metalaj a&ubreve;
      lignaj) latoj.
   </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_zh;&c_a;&c_l;&c_ju;&c_z;&c_ib;</trd>,
     <trd>&c_r;&c_a;&c_l;&c_mol;&c_sh;&c_t;&c_o;&c_r;&c_a;</trd>
   </trdgrp>
   <trd lng="de">Rollladen</trd>
   <trd lng="fr">volet roulant</trd>
   <trd lng="it">tapparella</trd>
   <trd lng="nl">rolluik</trd>
   <trd lng="pl">okiennica</trd>
</drv>
</art>

