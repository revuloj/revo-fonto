<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxutr.xml,v 1.11 2023/09/04 15:14:36 revo Exp $">
<kap>
  <rad>&scirc;utr</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="sxutr.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxutr.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Solida fenestro&scirc;irmilo, ekzemple el moveblaj tabuloj
      a&ubreve; 
      el ruleblaj latoj:
      <ekz>
        la vento fajfas kaj &scirc;ancelas <tld/>ojn
        <fnt><bib>Mary</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_a;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="de">Fensterladen</trd>
  <trd lng="en">window shutter</trd>
  <trd lng="fr">volet</trd>
  <trdgrp lng="hu">
    <trd>spaletta</trd>,
    <trd>ablakt&aacute;bla</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>imposta<klr> (serramento)</klr></trd>,
    <trd>persiana</trd>,
    <trd>scuro</trd>,
    <trd>tapparella</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30B7;&#x30E3;&#x30C3;&#x30BF;&#x30FC;</trd>,
    <trd>&#x3088;&#x308D;&#x3044;&#x6238;  [<ind>&#x3088;&#x308D;&#x3044;&#x3069;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>blind<klr tip="ind"> (luik)</klr></trd>,
    <trd>luik</trd>
  </trdgrp>
  <trd lng="pl">okiennica</trd>
  <trdgrp lng="pt">
    <trd>porta <klr>de janela</klr></trd>,
    <trd>contravento</trd>,
    <trd>guarda-vento</trd>
   </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x456;&#x43A;&#x43E;&#x43D;&#x43D;&#x438;&#x446;&#x44F;</trd>,
    <trd>&#x436;&#x430;&#x43B;&#x44E;&#x437;&#x456;</trd>,
    <trd>&#x437;&#x430;&#x442;&#x432;&#x43E;&#x440; (&#x443; &#x444;&#x43E;&#x442;&#x43E;&#x430;&#x43F;&#x430;&#x440;&#x430;&#x442;&#x456; &#x456; &#x442;.&#x43F;.)</trd>
  </trdgrp>
</drv>

<drv mrk="sxutr.rul0o">
  <kap>rul<tld/>o</kap>
  <snc>
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Fenestro&scirc;irmilo el ruleblaj (plastaj, metalaj a&ubreve;
      lignaj) latoj.
   </dif>
   </snc>
   <trd lng="fr">volet roulant</trd>
   <trd lng="it">tapparella</trd>
   <trd lng="nl">rolluik</trd>
   <trd lng="pl">okiennica</trd>
</drv>
</art>

