<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rotang.xml,v 1.6 2023/11/30 09:27:23 revo Exp $">
<kap>
  <rad>rotang</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="rotang.0o">
  <kap><tld/>o</kap>
  <snc mrk="rotang.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Sudazia palmo kun rektaj, malpezaj, fortikaj tigoj,
      el kiuj oni faras objektojn kaj meblojn,
      precipe el genro <trd lng="la">Calamus</trd>,
      sed anka&ubreve; el aliaj genroj de tribo <ctl>Calameae</ctl>:
      <ekz>
        la &ccirc;apo <klr>...</klr> estas farata el bambuo,
        folioj kaj <tld/>oj
        <fnt>
          <aut>-</aut>,
          <vrk>Silka vila&gcirc;o kaj konus&ccirc;apa vila&gcirc;o</vrk>,
          <lok><url ref="&CRI;other/magazine/dua_bulteno_2012.pdf">
          Universala Kongreso de Esperanto, Dua Bulteno,
          2012</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vimen.0o.BOT">vimeno</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_t;&c_a;&c_n;</trd>,
    <trd>&c_r;&c_a;&c_t;&c_a;&c_n;&c_g;</trd>
  </trdgrp>
  <trd lng="en">rattan</trd>
  <trd lng="fr">rotin</trd>
  <trd lng="ja">&#x30C8;&#x30A6;</trd>
  <trdgrp lng="pl">
    <trd>kalamus</trd>,
    <trd>rotang</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&#x43E;&c_t;&c_a;&#x43D;&c_g;</trd>,
    <trd>&c_r;&#x43E;&c_t;&c_a;&#x43D;</trd>,
    <trd>&c_r;&#x43E;&c_t;&c_a;&#x43D;&c_g;&#x43E;&c_v;&c_a; &#x43F;&c_a;&#x43B;&#x44C;&#x43C;&c_a;</trd>,
    <trd>&c_ib;&#x43D;&c_d;&c_ib;&c_j;&c_s;&#x44C;&#x43A;&c_a; &c_t;&c_r;&#x43E;&c_s;&c_t;&c_i;&#x43D;&c_a;</trd>
  </trdgrp>
</drv>
</art>

