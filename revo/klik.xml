<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: klik.xml,v 1.19 2022/05/01 08:09:24 revo Exp $">
<kap>
  <rad>klik</rad>/o
</kap>
<drv mrk="klik.0o">
  <kap><tld/>o</kap>
  <snc mrk="klik.0o.grupacxo">
    <uzo tip="fak">POL</uzo>
    <dif>
      Eta <ref tip="super" cel="grup.0acxo">grupa&ccirc;o</ref> de homoj, kiuj
      intrigas por malhelpi al aliaj kaj, ofte, bruas pri si por altiri
      &ccirc;ies atenton:
      <ekz>
        regantaj militistaj <tld/>oj en tiuj landoj
        <fnt>
          <aut>Ji&rcaron;&iacute; Patera</aut>,
          <vrk>Post la kunsido de financistoj en Prago</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2001a, numero 1a, p. 12a</lok>
        </fnt>;
      </ekz>
      <ekz>
        serboj <klr>[&#x2026;]</klr> ne plu fidis al propagando kaj vanaj promesoj de <nom>Milosevi&ccirc;</nom>
        kaj de lia <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>S. Maul</aut>
          <lok><url ref="&Steloj;monato2/007213.html">&Steloj;monato2/007213.html</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        militaj konfliktoj, interna malstabileco, anarkio a&ubreve; pro regantaj militistaj
        <tld/>oj en tiuj landoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Steloj;monato2/007258.html">Mondaj Financoj: Post la kunsido de financistoj en Prago</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        britoj ne plu toleros regadon de ri&ccirc;a, aroganta, indiferenta politika <tld/>o en
        Londono
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2014/011384.php">A&ccirc;etunto, atentu!</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="koteri.0o">koterio</ref>
    <refgrp tip="vid">
      <ref cel="kamari.0o">kamarilo</ref>,
      <ref cel="klan.0o">klano</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_k;&c_l;&c_ib;&c_k;&c_a;</trd>,
      <trd>&c_h;&c_a;&c_w;&c_r;&c_u;&c_s;</trd>
    </trdgrp>
    <trd lng="de">Kl&uuml;ngel</trd>
    <trdgrp lng="en">
      <trd>clique</trd>,
      <trd>coterie</trd>,
      <trd>set</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>clique</trd>,
      <trd>coterie</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>troep</trd>,
      <trd>bende</trd>
    </trdgrp>
    <trd lng="pl">klika</trd>
    <trd lng="ru">&c_k;&c_l;&c_i;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="klik.0o.grafeo">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>KompLeks</bib>, <lok>klikproblemo</lok></fnt>
    <dif>
      <klr>(de <ref cel="grafe.0o.strukturo">grafeo</ref>)</klr>
      <ref cel="komple2.0a.grafeo">Kompleta</ref>
      <ref cel="grafe.sub0o">subgrafeo</ref> de &gcirc;i.
    </dif>
    <trdgrp lng="de">
      <trd>Clique</trd>,
      <trd>Sippe</trd>
    </trdgrp>
    <trd lng="en">clique</trd>
    <trd lng="fr">clique<klr tip="ind"> (d'un graphe)</klr></trd>
    <trd lng="pl">klika<klr tip="ind"> (grafu)</klr></trd>
    <trd lng="ru">&c_k;&c_l;&c_i;&c_k;&c_a;</trd>
  </snc>
  <rim>
    <ref tip="hom" cel="klik1.0o">Kliko</ref> estas anka&ubreve; intermetilo de dentrado.
  </rim>
  <trdgrp lng="sk">
    <trd>z&aacute;padka</trd>,
    <trd>z&aacute;str&ccaron;ka</trd>,
    <trd>z&aacute;vora</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>z&aacute;padka</trd>,
    <trd>z&aacute;str&ccaron;ka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x441;&#x442;&#x43E;&#x43F;&#x43E;&#x440;</trd>,
    <trd>&#x441;&#x43E;&#x431;&#x430;&#x447;&#x43A;&#x430;</trd>,
    <trd>&#x437;&#x430;&#x441;&#x443;&#x432;&#x43A;&#x430;</trd>,
    <trd>&#x437;&#x430;&#x441;&#x43A;&#x43E;&#x447;&#x43A;&#x430;</trd>,
    <trd>&#x43A;&#x43B;&#x456;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>
</art>
