      Banda&ccirc;o.  
    </dif>
    <trdgrp lng="en">
      <trd>clique</trd>,
      <trd>coterie</trd>,
      <trd>set</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_l;&c_i;&c_k;&c_a;</trd>
  </snc>
</drv>
