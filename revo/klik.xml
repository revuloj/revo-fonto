<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: klik.xml,v 1.24 2023/12/03 09:58:50 revo Exp $">
<kap>
  <rad>klik</rad>/o
</kap>
<drv mrk="klik.0o">
  <kap><tld/>o</kap>
  <snc mrk="klik.0o.grupacxo">
    <uzo tip="fak">POL</uzo>
    <dif>
      Eta <ref tip="super" cel="grup.0acxo">grupa&ccirc;o</ref> de homoj, kiuj
      intrigas por malhelpi al aliaj kaj, ofte, bruas pri si por altiri
      &ccirc;ies atenton:
      <ekz>
        regantaj militistaj <tld/>oj en tiuj landoj
        <fnt>
          <aut>Ji&rcaron;&iacute; Patera</aut>,
          <vrk>Post la kunsido de financistoj en Prago</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2001a, numero 1a, p. 12a</lok>
        </fnt>;
      </ekz>
      <ekz>
        serboj <klr>[&#x2026;]</klr> ne plu fidis al propagando kaj vanaj promesoj de <nom>Milosevi&ccirc;</nom>
        kaj de lia <tld/>o
        <fnt>
          <aut>Stefan Maul</aut>
            <vrk><url ref="&Monato;/2000/007213.php">Estimataj gelegantoj,</url></vrk>
            <bib>Monato</bib>
            <lok>2000/11, p. 3</lok>
          </fnt>;
      </ekz>
      <ekz>
        militaj konfliktoj, interna malstabileco, anarkio a&ubreve; pro regantaj militistaj
        <tld/>oj en tiuj landoj
        <fnt>
       <aut>Ji&rcaron;&iacute; Patera</aut>
          <vrk><url ref="&Monato;/2001/007258.html">Post la kunsido de financistoj en
          Prago</url></vrk>
          <bib>Monato</bib>
          <lok>2001/01, p. 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        britoj ne plu toleros regadon de ri&ccirc;a, aroganta, indiferenta politika <tld/>o en
        Londono
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2014/011384.php">A&ccirc;etunto, atentu!</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="koteri.0o">koterio</ref>
    <refgrp tip="vid">
      <ref cel="kamari.0o">kamarilo</ref>,
      <ref cel="klan.0o">klano</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_k;&c_l;&c_ib;&c_k;&c_a;</trd>,
      <trd>&c_h;&c_a;&c_w;&c_r;&c_u;&c_s;</trd>
    </trdgrp>
    <trd lng="de">Kl&uuml;ngel</trd>
    <trdgrp lng="en">
      <trd>clique</trd>,
      <trd>coterie</trd>,
      <trd>set</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>clique</trd>,
      <trd>coterie</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>troep</trd>,
      <trd>bende</trd>
    </trdgrp>
    <trd lng="pl">klika</trd>
    <trd lng="ru">&c_k;&c_l;&c_i;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="klik.0o.grafeo">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>KompLeks</bib>, <lok>klikproblemo</lok></fnt>
    <dif>
      <klr>(de <ref cel="grafe.0o.strukturo">grafeo</ref>)</klr>
      <ref cel="komple2.0a.grafeo">Kompleta</ref>
      <ref cel="grafe.sub0o">subgrafeo</ref> de &gcirc;i.
    </dif>
    <trdgrp lng="de">
      <trd>Clique</trd>,
      <trd>Sippe</trd>
    </trdgrp>
    <trd lng="en">clique</trd>
    <trd lng="fr">clique<klr tip="ind"> (d'un graphe)</klr></trd>
    <trd lng="pl">klika<klr tip="ind"> (grafu)</klr></trd>
    <trd lng="ru">&c_k;&c_l;&c_i;&c_k;&c_a;</trd>
  </snc>
  <rim>
    <ref tip="hom" cel="klik1.0o">Kliko</ref> estas anka&ubreve; intermetilo de dentrado.
  </rim>
  <trdgrp lng="ja">
    <trd>&#x6B6F;&#x6B62;&#x3081;  <pr>&#x306F;&#x3069;&#x3081;</pr></trd>,
    <trd>&#x722A;  <pr>&#x3064;&#x3081;</pr></trd>,
    <trd>&#x5F92;&#x515A;  <pr>&#x3068;&#x3068;&#x3046;</pr></trd>,
    <trd>&#x4E00;&#x5473;  <pr>&#x3072;&#x3068;&#x3042;&#x3058;</pr></trd>,
    <trd>&#x6D3E;&#x95A5;  <pr>&#x306F;&#x3070;&#x3064;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>z&aacute;padka</trd>,
    <trd>z&aacute;str&ccaron;ka</trd>,
    <trd>z&aacute;vora</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>z&aacute;padka</trd>,
    <trd>z&aacute;str&ccaron;ka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&#x43E;&#x43F;&#x43E;&c_r;</trd>,
    <trd>&c_s;&#x43E;&c_b;&c_a;&c_ch;&#x43A;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_u;&c_v;&#x43A;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_s;&#x43A;&#x43E;&c_ch;&#x43A;&c_a;</trd>,
    <trd>&#x43A;&#x43B;&c_ib;&#x43A;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5C0F;&#x96C6;&#x56E2; <pr>xi&#x1CE;oj&#xED;tu&#xE1;n</pr></trd>,
    <trd>&#x95A5; <pr>f&#xE1;</pr></trd>,
    <trd>&#x670B;&#x515A; <pr>p&#xE9;ngd&#x1CE;ng</pr></trd>,
    <trd>&#x5B97;&#x6D3E; <pr>z&#x14D;ngp&#xE0;i</pr></trd>,
    <trd>&#x5E2E;&#x6D3E; <pr>b&#x101;ngp&#xE0;i</pr></trd>,
    <trd>&#x79C1;&#x515A; <pr>s&#x12B;d&#x1CE;ng</pr></trd>
  </trdgrp>
</drv>
</art>
