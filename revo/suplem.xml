<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: suplem.xml,v 1.37 2023/10/21 07:46:48 revo Exp $">
<kap>
  <ofc>8</ofc> <rad>suplement</rad>/o 
  <fnt><aut>K. Bennemann</aut>, <vrk>Esperanto Hand-W&ouml;rterbuch</vrk>, <lok>1923-1926</lok></fnt>
  <fnt> <bib>PV</bib></fnt>
</kap>
<drv mrk="suplem.0o">
  <kap><tld/>o</kap>
  <snc mrk="suplem.0o.KOMUNE">
    <dif>
      Kompletiga <ref tip="super" cel="don.al0o.ajxo">aldono</ref>:
      <ekz>
        <uzo tip="fak">GRA</uzo>
        <tld/>o de participsubstantivo
        <fnt>
          <bib>LR</bib>
          <lok>11. Verboj</lok>
        </fnt>;
      </ekz>
      <ekz>
        literatura <tld/>o al bulteno
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2004/008398.html">Ankora&ubreve;foje:
          retrorigardo</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu terminaro estas <tld/>o al la ekzistantaj terminaroj pri ekonomiko
        <fnt>
          <bib>Monato</bib>
          <aut>Zlatko Hin&scaron;t</aut>
          <vrk><url ref="&Monato;/2005/008517.php">Suplemento al ekonomiaj
          terminaroj</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        nutraj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van De Velde</aut>
          <vrk><url ref="&Monato;/2013/010934.php">Sablohorlo&gcirc;o for&scirc;ovas
          piramidon</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        engluti &ccirc;iutage pilolojn da vitaminoj kaj mineraloj <klr>[per]</klr> sintezitaj
        <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Paulo S&eacute;rgio Viana</aut>
          <vrk><url ref="&Monato;/2014/011267.php">Iam kompletigaj, kelkfoje
          malsanigaj</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        estu alkalkulita <tld/>o <klr>(aldonpago)</klr>
        por ku&scirc;loko en nokta trajno
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2007/009272.php">Malfavore fervoje</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        la plej granda <tld/>o <klr>(kromrezulto)</klr> al 
        kontra&ubreve;rabia vakcino estis eltrovo kaj akirado
        <klr>[&#x2026;]</klr> de specifa kontra&ubreve;rabia imunoglobulino
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2008/009550.php">Batalo kontra&ubreve; mortiga
          malsano</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="krom.0ajxo">kroma&jcirc;o</ref>
    <refgrp tip="vid">
       <ref cel="bonus.0o">bonuso</ref>,
       <ref cel="komple2.0igi">kompletig</ref>a&jcirc;o,
       <ref cel="komple1.0o">komplemento</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_d;&c_a;&c_b;&c_a;&c_w;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>, 
      <trd>&c_d;&c_a;&c_d;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>, 
      <trd>&c_d;&c_a;&c_d;&c_a;&c_t;&c_a;&c_k;</trd>, 
      <trd>&c_d;&c_a;&c_p;&c_a;&c_w;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trd lng="ca">suplement</trd>
    <trdgrp lng="de">
      <trd>Supplement</trd>,
      <trd>Erg&auml;nzung</trd>,
      <trd>Zusatz</trd>
    </trdgrp>
    <trd lng="en">supplement</trd>
    <trd lng="fr">suppl&eacute;ment</trd>
    <trdgrp lng="hu">
      <trd>kieg&eacute;sz&iacute;t&eacute;s</trd>, 
      <trd>p&oacute;tl&aacute;s</trd>, 
      <trd>szupplementum</trd>
    </trdgrp>
   <trd lng="it">supplemento</trd>      
    <trd lng="ru">&c_d;&c_o;&c_b;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="suplem.0o.angulo">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      <klr>(de <ref cel="angul.0o.figuro">angulo</ref>)</klr>
      Angulo <tld/>a<sncref ref="suplem.0a.angulo"/> al &gcirc;i:
      <ekz>
        en triangulo, &ccirc;iu angulo estas <tld/>o de la sumo
        de la du aliaj;
      </ekz>
      <ekz>
        sinuso de la <tld/>o de angulo egalas al sinuso de la
        originalo <klr>(<frm am="sin(pi-theta)=sin theta">sin(&pi;-&theta;) = sin&theta;</frm>)</klr>.
      </ekz>
    </dif>
    <trd lng="be">&c_d;&c_a;&c_d;&c_a;&c_t;&c_k;&c_o;&c_v;&c_y;
<ind>&c_v;&c_u;&c_g;&c_a;&c_l;</ind></trd>
    <trd lng="ca">angle suplementari</trd>
    <trd lng="de">Supplementwinkel</trd>
    <trd lng="en">supplementary <ind>angle</ind></trd>
    <trdgrp lng="fr">
      <trd>angle suppl&eacute;mentaire</trd>,
      <trd>suppl&eacute;mentaire<klr tip="ind"> (d'un angle)</klr></trd>
    </trdgrp>
    <trd lng="hu">kieg&eacute;sz&iacute;t&odblac; <ind>sz&ouml;g</ind></trd>    
    <trd lng="ru">&c_d;&c_o;&c_p;&c_o;&c_l;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;
<ind>&c_u;&c_g;&c_o;&c_l;</ind></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>dodatek</trd>,
    <trd>dopln&ecaron;k</trd>,
    <trd>p&rcaron;&iacute;davek</trd>,
    <trd>suplement</trd>,
    <trd>suplementum</trd>
  </trdgrp>
  <trd lng="es">suplemento</trd>
  <trdgrp lng="ja">
    <trd>&#x88DC;&#x8DB3;  <pr>&#x307B;&#x305D;&#x304F;</pr></trd>,
    <trd>&#x8FFD;&#x52A0;  <pr>&#x3064;&#x3044;&#x304B;</pr></trd>,
    <trd>&#x88DC;&#x907A;  <pr>&#x307B;&#x3044;</pr></trd>,
    <trd>&#x4ED8;&#x9332;  <pr>&#x3075;&#x308D;&#x304F;</pr></trd>,
    <trd>&#x88DC;&#x8DB3;&#x8A9E;  <pr>&#x307B;&#x305D;&#x304F;&#x3054;</pr></trd>,
    <trd>&#x88DC;&#x89D2;  <pr>&#x307B;&#x304B;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">supplement</trd>
  <trd lng="ru">&c_d;&c_o;&c_p;&c_o;&c_l;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>dodatok</trd>,
    <trd>dolo&zcaron;ka</trd>,
    <trd>doplnok</trd>,
    <trd>pr&iacute;davok</trd>,
    <trd>suplement</trd>
  </trdgrp>
  <trd lng="uk">&#x43B;&#x456;&#x442;&#x435;&#x440;&#x430;&#x442;&#x443;&#x440;&#x43D;&#x438;&#x439; &#x434;&#x43E;&#x434;&#x430;&#x442;&#x43E;&#x43A;</trd>
</drv>

<drv mrk="suplem.0a">
  <kap><tld/>a</kap>
  <snc mrk="suplem.0a.KOMUNE">
    <dif>
      Rilata al <tld/>o<sncref ref="suplem.0o.KOMUNE"/>, aldona,
      kompletiga, <ref tip="vid" cel="krom.0a.KOMUNE">kroma</ref>:
      <ekz>
        <tld/>a noto;
      </ekz>
      <ekz>
        <uzo tip="fak">EKON</uzo>
        <ind><tld/>aj horoj</ind> <fnt><bib>PIV1</bib></fnt>;
        <trd lng="de">&Uuml;berstunden</trd>
        <trd lng="en">overtime</trd>
        <trd lng="fr">heures <ind>suppl&eacute;mentaires</ind></trd>
        <trd lng="hu">t&uacute;l&oacute;r&aacute;k</trd>        
      </ekz>
      <ekz>
        diet-<tld/>a terapio
        <fnt>
          <bib>Monato</bib>
          <aut>Paulo S&eacute;rgio Viana</aut>
          <vrk><url ref="&Monato;/2014/011267.php">Iam kompletigaj, kelkfoje
          malsanigaj</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        gitaro estas la baza instrumento, sed <tld/>e sonas pluraj pliaj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan MacGill</aut>
          <vrk><url ref="&Monato;/2007/009035.php">Knabinoj, cignoj,
          bur&gcirc;onoj</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu kalendaro enhavas dek du tridek-tagajn monatojn, <klr>[&#x2026;]</klr> al kiuj &gcirc;i
        aldonas kvin (a&ubreve; ses &ccirc;e superjaroj) <tld/>ajn jarfinajn tagojn por festi la
        jar&scirc;an&gcirc;on
        <fnt>
          <bib>Monato</bib>
          <aut>Thierry Tailhades</aut>
          <vrk><url ref="&Monato;/2015/011489.php">Kalendaroj (2)</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="akceso.0a">akcesora</ref>
    <trdgrp lng="be">
      <trd>&c_d;&c_a;&c_d;&c_a;&c_t;&c_k;&c_o;&c_v;&c_y;</trd>, 
      <trd>&c_d;&c_a;&c_p;&c_a;&c_w;&c_n;&c_ja;&c_l;&c_mol;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>zus&auml;tzlich</trd>,
      <trd>erg&auml;nzend</trd>
    </trdgrp>
    <trd lng="en">supplementary</trd>
    <trd lng="fr">suppl&eacute;mentaire<klr tip="ind"> (adj.)</klr></trd>
  </snc>
  <snc mrk="suplem.0a.angulo">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 27</lok></fnt>
    <dif>
      <klr>(pri du <ref cel="angul.0o.figuro">anguloj</ref>)</klr>
      Kies sumo egalas al <ref cel="strecx.0ita.angulo">stre&ccirc;ita</ref>
      angulo:
      <ekz>
        <tld/>aj anguloj havas la saman sinuson kaj kontra&ubreve;egalan
        kosinuson.
      </ekz>
    </dif>
    <trd lng="be">&c_d;&c_a;&c_d;&c_a;&c_t;&c_k;&c_o;&c_v;&c_y;<klr tip="ind"> (&c_v;&c_u;&c_g;&c_a;&c_l;)</klr></trd>
    <trd lng="de">Supplement-<klr tip="amb">(winkel)</klr></trd>
    <trd lng="en">supplementary<klr tip="ind"> (angle)</klr></trd>
    <trd lng="fr"><klr tip="ind">(angle) </klr>suppl&eacute;mentaire</trd>
    <trd lng="ru">&c_d;&c_o;&c_p;&c_o;&c_l;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;<klr tip="ind"> (&c_u;&c_g;&c_o;&c_l;)</klr></trd>
  </snc>
  <trd lng="ca">suplementari</trd>
  <trdgrp lng="cs">
    <trd>dopl&ncaron;kov&yacute;</trd>,
    <trd>v&yacute;pl&ncaron;kov&yacute;</trd>
  </trdgrp>
  <trd lng="es">suplementario</trd>
  <trdgrp lng="hu">
    <trd>kieg&eacute;sz&iacute;t&odblac;</trd>,
    <trd>p&oacute;t-</trd>
  </trdgrp>
  <trd lng="it">supplementare</trd>
  <trdgrp lng="ja">
    <trd>&#x88DC;&#x8DB3;&#x306E;  <pr>&#x307B;&#x305D;&#x304F;&#x306E;</pr></trd>,
    <trd>&#x8FFD;&#x52A0;&#x306E;  <pr>&#x3064;&#x3044;&#x304B;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_o;&c_p;&c_o;&c_l;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>dodatkov&yacute;</trd>,
    <trd>dodato&ccaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="uk">&#x434;&#x43E;&#x434;&#x430;&#x442;&#x43A;&#x43E;&#x432;&#x438;&#x439;</trd>
</drv>

<drv mrk="suplem.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="suplem.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="komple2.0igi">Kompletigi</ref> per <tld/>o:
      <ekz>
        la du sencaj tavoloj de la verko &ndash; la lirika kaj pamfleta &ndash; <klr>[&#x2026;]</klr> trafe
        <tld/>as unu la alian
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="&Monato;/2011/010234.php">Lirika politika pamfleto</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiuj vidpunktoj devas <tld/>i unu la alian kaj prezenti siaspecan
        &leftquot;stereoskopian&rightquot; bildon de la afero
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="&Monato;/2003/007814.html">Malnova biblia epizodo en modernaj
          vestoj</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la kompanio]</klr> <tld/>as <klr>[&#x2026;]</klr> mizeran salajron per kroma
        laboro
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2015/011468.php">Unu regulo por ri&ccirc;uloj, dua regulo
          por malri&ccirc;uloj</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        bona dieto de naturaj substancoj, <tld/>ata per iom
        da sunlumo, liveras &ccirc;iujn bezonatajn vitaminojn
        <fnt><aut>W. F. Rolt</aut>, <vrk>El la Polvo de la Tero</vrk>, 
        <lok>1967</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>erg&auml;nzen</trd>,
      <trd>supplementieren</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_p;&c_a;&c_w;&c_n;&c_ja;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>suplementar</trd>,
    <trd>complementar</trd>
  </trdgrp>
  <trd lng="fr">compl&eacute;ter</trd>
</drv>
</art>
