<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>8</ofc> <rad>suplement</rad>/o <fnt>N</fnt></kap>
<drv mrk="suplem.0o">
  <kap><tld/>o</kap>
  <snc mrk="suplem.0o.KOMUNE">
    <dif>
      Kompletiga aldono.
    </dif>
    <trdgrp lng="de">
      <trd>Supplement</trd>,
      <trd>Erg&auml;nzung</trd>,
      <trd>Zusatz</trd>
    </trdgrp>
    <trd lng="fr">suppl&eacute;ment</trd>
  </snc>
  <snc mrk="suplem.0o.angulo">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      <klr>(de <ref tip="super" cel="angul.0o">angulo</ref>)</klr>
      Angulo <ref cel="suplem.0a.angulo"><tld/>a</ref> al &gcirc;i:
      <ekz>
        en triangulo, &ccirc;iu angulo estas <tld/>o de la sumo
        de la du aliaj;
      </ekz>
      <ekz>
        sinuso de la <tld/>o de angulo egalas al sinuso de la
        originalo <klr>(sin(&pi;-&theta;) = sin&theta;)</klr>.
      </ekz>
    </dif>
    <trd lng="fr">suppl&eacute;mentaire<klr tip="ind"> (d'un angle)</klr></trd>
  </snc>
  <trd lng="nl">supplement</trd>
</drv>

<drv mrk="suplem.0a">
  <kap><tld/>a</kap>
  <snc mrk="suplem.0a.KOMUNE">
    <dif>
      Rilata al <tld/>o <sncref ref="suplem.0o.KOMUNE"/>, aldona, kompletiga:
      <ekz>
        <tld/> noto;
      </ekz>
      <ekz>
        <uzo tip="fak">EKON</uzo>
        <ind><tld/>aj horoj</ind> <fnt><bib>PIV1</bib></fnt>.
        <trd lng="fr">heures <ind>suppl&eacute;mentaires</ind></trd>
      </ekz>
    </dif>
    <trd lng="fr">suppl&eacute;mentaire<klr tip="ind"> (adj.)</klr></trd>
  </snc>
  <snc mrk="suplem.0a.angulo">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      <klr>(p.p. du <ref cel="angul.0o.figuro">anguloj</ref>)</klr>
      Kies sumo egalas al <ref cel="strecx.0ita.angulo">stre&ccirc;ita</ref>
      angulo:
      <ekz>
        <tld/>aj anguloj havas la saman sinuson kaj kontra&ubreve;egalan
        kosinuson.
      </ekz>
    </dif>
    <trd lng="de">Supplement-<klr tip="amb">(-winkel)</klr></trd>
    <trd lng="en">supplementary<klr tip="ind"> (angle)</klr></trd>
    <trd lng="fr"><klr tip="ind">(angle) </klr>suppl&eacute;mentaire</trd>
    <trd lng="ru">&c_d;&c_o;&c_p;&c_o;&c_l;&c_n;&c_i;&c_t;&c_je;&c_l;&c_n;&c_y;&c_j;<klr tip="ind"> (&c_u;&c_g;&c_o;&c_l;)</klr></trd>
    <adm>
    Kontroli trd ru: sxajnas, ke ankaux "komplementa" tradukigxas
    tiel. Cxu gxuste? <aut>MB</aut>
    </adm>
  </snc>
</drv>
</art>
