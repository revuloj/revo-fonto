<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>spiral</rad>/o
</kap>
<drv mrk="spiral.0o">
  <kap><tld/>o</kap>
  <snc mrk="spiral.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 35</lok></fnt>
    <uzo tip="klr">(nerigora difino)</uzo>
    <dif>
      Ebena <ref tip="lst" lst="voko:formoj" cel="kurb.0o.MAT">kurbo</ref>, prezentanta
      la trajektorion de punkto, kiu rondiras &ccirc;irka&ubreve; fiksa
      punkto kaj samtempe malproksimi&gcirc;as de &gcirc;i.
    </dif>
    <refgrp tip="sub">
      <ref cel="spiral.arhximeda0o">ar&hcirc;imeda <tld/>o</ref>,
      <ref cel="spiral.logaritma0o">logaritma <tld/>o</ref>;
    </refgrp>
    <refgrp tip="vid">
      Ne konfuzu kun:
      <ref cel="helic.0o.MAT">helico</ref>.
    </refgrp>
  </snc>  
  <snc mrk="spiral.0o.KOMUNE">
    <dif>
      Kurbo pli-malpli samforma, ofte neebena, foje e&ccirc;
      en formo de <ref cel="helic.0o.MAT">helico</ref>:

      <ekz>
        la spiralo de &scirc;tuparo en turo
        <fnt><bib>PIV1</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Dio faligas ilin tra la <tld/>o de la tempo
        <fnt><bib>Fab4</bib>, <lok>Pupludisto</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>o de la salajroj kaj de la prezoj en inflacia procezo
        <fnt><bib>PIV1</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>a konko de heliko;
      </ekz>
      <ekz>
        <tld/>a vojo, &scirc;tuparo;
      </ekz>
      <ekz>
        &scirc;ranko kun skulptitaj <tld/>a&jcirc;oj kaj folia&jcirc;oj
        <fnt><bib>Fab2</bib>, <lok>Pa&scirc;tistino kaj
        kamentubisto</lok></fnt>;
      </ekz>
      <ekz>
        a&ubreve;tuna vento pelis mortintajn foliojn kaj turnadis ilin <tld/>e
        &ccirc;irka&ubreve; nevidebla akso.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_p;&c_ib;&c_r;&c_a;&c_l;&c_mol;</trd>
  <trd lng="ca">espiral</trd>
  <trd lng="cs">spir&aacute;la</trd>
  <trd lng="de">Spirale</trd>
  <trd lng="en">spiral</trd>
  <trd lng="es">espiral</trd>
  <trd lng="fr">spirale</trd>
  <trdgrp lng="hu">
    <trd>spir&aacute;l</trd>,
    <trd>csavarvonal</trd>,
    <trd>csigavonal</trd>
  </trdgrp>
  <trd lng="pl">spirala</trd>
  <trd lng="ru">&c_s;&c_p;&c_i;&c_r;&c_a;&c_l;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>z&aacute;vit</trd>,
    <trd>&scaron;pir&aacute;la</trd>
  </trdgrp>
</drv>

<drv mrk="spiral.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Havanta proksimuman formon de <tld/>o:
      <ekz>
        <tld/>a galaksio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref='https://eo.wikipedia.org/?curid=199491'>Klasifiko de galaksioj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">spiralf&ouml;rmig</trd>
  </snc>
</drv>

<drv mrk="spiral.arhximeda0o">
  <kap>ar&hcirc;imeda <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="spiral.0o.MAT"><tld lit="S"/>o
      <sncref/></ref>,
      kies polusa <ref cel="ekvaci.0o">ekvacio</ref> estas de la tipo
      <frm>&rho; = <k>k</k>&theta;</frm>.
    </dif>
    <refgrp tip="vid">
      <ref cel="arhxim.0o">Ar&hcirc;imedo</ref>.
    </refgrp>
  </snc>
  <bld lok="../bld/k_spiralo1.svg.gif" alt="300" lrg="300">
    ar&hcirc;imeda <tld/>o
  </bld>
  <trd lng="be">&c_a;&c_r;&c_h;&c_ib;&c_m;&c_e;&c_d;&c_a;&c_v;&c_a;&c_ja; 
    <ind>&c_s;&c_mol;&c_p;&c_ib;&c_r;&c_a;&c_l;&c_mol;</ind></trd>
  <trd lng="de">Archimedische <ind>Spirale</ind></trd>
  <trd lng="en">Archimedes['] <ind>spiral</ind></trd>
  <trd lng="fr">spirale d'Archim&egrave;de</trd>
  <trd lng="hu">archimedesi <ind>spir&aacute;l</ind></trd>
  <trd lng="pl">spirala Archimedesa</trd>
  <trd lng="ru">&c_a;&c_r;&c_h;&c_i;&c_m;&c_je;&c_d;&c_o;&c_v;&c_a; <ind>&c_s;&c_p;&c_i;&c_r;&c_a;&c_l;&c_mol;</ind></trd>
</drv>

<drv mrk="spiral.hiperbola0o">
  <kap>hiperbola <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="spiral.0o.MAT"><tld lit="S"/>o
      <sncref/></ref>, 
      kies polusa <ref cel="ekvaci.0o">ekvacio</ref> estas de la tipo
      <frm>&rho; = <k>k</k>/&theta;</frm>:
      <ekz>
        hiperbola <tld/>o estas transcenda ebena kurbo anka&ubreve; sciata kiel reciproka <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref='https://eo.wikipedia.org/?curid=83333'>Hiperbola spiralo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/k_spiralo3.svg.gif" alt="300" lrg="300">
    hiperbola <tld/>o
  </bld>
  <adm>Fonto trovenda. <aut>MB</aut></adm>
  <trd lng="de">hyperbolische <ind>Spirale</ind></trd>
  <trd lng="en">hyperbolic <ind>spiral</ind></trd>
  <trd lng="fr">spirale hyperbolique</trd>
  <trd lng="pl">spirala hiperboliczna</trd>
  <trd lng="ru">&c_g;&c_i;&c_p;&c_je;&c_r;&c_b;&c_o;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja;
  <ind>&c_s;&c_p;&c_i;&c_r;&c_a;&c_l;&c_mol;</ind></trd>
</drv>

<drv mrk="spiral.logaritma0o">
  <kap>logaritma <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="spiral.0o.MAT"><tld lit="S"/>o
      <sncref/></ref>, 
      kies polusa <ref cel="ekvaci.0o">ekvacio</ref> estas de la tipo
      <frm>&rho; = <k>e</k><sup><k>k</k>&theta;</sup></frm>.
    </dif>
    <refgrp tip="vid">
      <ref cel="logari.0o">logaritmo</ref>.
    </refgrp>
  </snc>
  <bld lok="../bld/k_spiralo2.svg.gif" alt="300" lrg="300">
    logaritma <tld/>o
  </bld>
  <adm>Fonto trovenda. <aut>MB</aut></adm>
  <trd lng="be">&c_l;&c_ja;&c_g;&c_a;&c_r;&c_y;&c_t;&c_m;&c_ib;&c_ch;&c_n;&c_a;&c_ja; 
    <ind>&c_s;&c_mol;&c_p;&c_ib;&c_r;&c_a;&c_l;&c_mol;</ind></trd>
  <trd lng="de">logarithmische <ind>Spirale</ind></trd>
  <trd lng="en">logarithmic <ind>spiral</ind></trd>
  <trd lng="fr">spirale logarithmique</trd>
  <trd lng="hu">logaritmikus <ind>spir&aacute;l</ind></trd>
  <trd lng="pl">spirala logarytmiczna</trd>
  <trd lng="ru">&c_l;&c_o;&c_g;&c_a;&c_r;&c_i;&c_f;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja;
  <ind>&c_s;&c_p;&c_i;&c_r;&c_a;&c_l;&c_mol;</ind></trd>
</drv>
</art>
