<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ektropi</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="ektrop.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Renversa eksteri&gcirc;o de palpebra <ref cel="mukoz.0o">mukozo</ref>.
    </dif>
    <adm>
      Mi anstata&ubreve;igis la nedifinitan "mukmembrano" per "mukozo".
      REVO konas anka&ubreve; "muka membrano". <aut>MB</aut>
    </adm>
    <refgrp tip="vid">
      <ref cel="entrop.0o">entropio</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Ektropium</trd>,
      <trd>Ausw&auml;rtskehrung</trd>,
      <trd>Ausst&uuml;lpung</trd>
    </trdgrp>
    <trd lng="es">ectropi&oacute;n</trd>
    <trd lng="fr">ectropion</trd>
    <trdgrp lng="de">
      <trd>ectropium</trd>,
      <trd>kifordul&aacute;s</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_e;&c_k;&c_t;&c_r;&c_o;&c_p;&c_i;&c_o;&c_n;</trd>,
      <trd>&c_e;&c_k;&c_t;&c_r;&c_o;&c_p;&c_i;&c_ja;</trd>,
      <trd>&c_v;&c_y;&c_v;&c_o;&c_r;&c_o;&c_t; &c_v;&c_je;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
</drv>
</art>
