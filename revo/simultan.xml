<art mrk="$Id: simultan.xml,v 1.10 2025/04/05 16:29:35 revo Exp $">
<kap>
  <rad>simultan</rad>/a <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="simultan.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="simultan.0a.KOMUNE">
    <dif>
      <ref tip="dif" cel="temp.sam0a.KOMUNE">samtempa</ref>,
      sammomenta:
      <ekz>
        sentu tion nun, &ccirc;i sekunde, <tld/>e kun mi
        <fnt><aut>S. Larbar</aut>, <vrk>Karuseloj</vrk>, 
        <lok>1987</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>samtempa</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_n;&c_a;&c_ch;&c_a;&c_s;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_a;&c_d;&c_n;&c_a;&c_ch;&c_a;&c_s;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="en">simultaneous</trd>
  <trdgrp lng="cs">
    <trd>simult&aacute;nn&iacute;</trd>,
    <trd>soub&ecaron;&zcaron;n&yacute;</trd>,
    <trd>soudob&yacute;</trd>
  </trdgrp>
  <trd lng="fr">concomitant</trd>
  <trdgrp lng="pl">
    <trd>symultaniczny</trd>,
    <trd>jednoczesny</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>simult&aacute;nny</trd>,
    <trd>s&uacute;be&zcaron;n&yacute;</trd>,
    <trd>s&uacute;&ccaron;asn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#1086;&c_d;&#1085;&#1086;&c_ch;&c_a;&c_s;&#1085;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_i;&#1085;&c_h;&c_r;&#1086;&#1085;&#1085;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#24179;&#34892; <pr>p&#237;ngx&#237;ng</pr></trd>,
    <trd>&#21516;&#26102; <pr>t&#243;ngsh&#237;</pr></trd>,
    <trd>&#21516;&#26178; <pr>t&#243;ngsh&#237;</pr></trd>,
    <trd>&#19968;&#38754; <pr>y&#299;mi&#224;n</pr></trd>
  </trdgrp>
</drv>
