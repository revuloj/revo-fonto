<art mrk="$Id: simultan.xml,v 1.8 2024/03/24 20:21:57 revo Exp $">
<kap>
  <rad>simultan</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="simultan.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="simultan.0a.KOMUNE">
    <dif>
      <ref tip="dif" cel="temp.sam0a.KOMUNE">samtempa</ref>,
      sammomenta:
      <ekz>
        sentu tion nun, &ccirc;i sekunde, <tld/>e kun mi
        <fnt><aut>S. Larbar</aut>, <vrk>Karuseloj</vrk>, 
        <lok>1987</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_n;&c_a;&c_ch;&c_a;&c_s;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_a;&c_d;&c_n;&c_a;&c_ch;&c_a;&c_s;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="en">simultaneous</trd>
  <trdgrp lng="cs">
    <trd>simult&aacute;nn&iacute;</trd>,
    <trd>soub&ecaron;&zcaron;n&yacute;</trd>,
    <trd>soudob&yacute;</trd>
  </trdgrp>
  <trd lng="fr">concomitant</trd>
  <trdgrp lng="pl">
    <trd>symultaniczny</trd>,
    <trd>jednoczesny</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>simult&aacute;nny</trd>,
    <trd>s&uacute;be&zcaron;n&yacute;</trd>,
    <trd>s&uacute;&ccaron;asn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43E;&c_d;&#x43D;&#x43E;&c_ch;&c_a;&c_s;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_i;&#x43D;&c_h;&c_r;&#x43E;&#x43D;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5E73;&#x884C; <pr>p&#xED;ngx&#xED;ng</pr></trd>,
    <trd>&#x540C;&#x65F6; <pr>t&#xF3;ngsh&#xED;</pr></trd>,
    <trd>&#x540C;&#x6642; <pr>t&#xF3;ngsh&#xED;</pr></trd>,
    <trd>&#x4E00;&#x9762; <pr>y&#x12B;mi&#xE0;n</pr></trd>
  </trdgrp>
</drv>
</art>

