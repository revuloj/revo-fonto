<kap>
  <rad>malign</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="malign.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="malign.0a.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Ka&scirc;e kaj dan&gcirc;ere <ref tip="super" cel="util.mal0a">malutila</ref>, 
      insidema:
      <ekz>
        ni faras justan militon <klr>...</klr> ekstermante tiel <tld/>an
        rason
        <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 8a, p.
        65a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_l;&c_a;&c_m;&c_y;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_z;&c_l;&c_o;&c_k;&c_a;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;</trd>
  <trd lng="ca">maligne</trd>
  <trdgrp lng="cs">
    <trd>zhoubn&yacute;</trd>,
    <trd>&scaron;kodliv&yacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>malicious</trd>,
    <trd>malignant</trd>
  </trdgrp>
  <trd lng="fr">malin<klr> (nuisible)</klr></trd>
  <trd lng="hu">rosszindulat&uacute;<klr tip="ind"> (pl. daganat)</klr></trd>
  <trd lng="ja">&#x60AA;&#x6027;&#x306E;  <pr>&#x3042;&#x304F;&#x305B;&#x3044;&#x306E;</pr></trd>
  <trd lng="nl">kwaadaardig</trd>
  <trd lng="pl">maj&aogonek;cy z&lstroke;e rokowania</trd>
  <trd lng="pt">maligno</trd>
  <trd lng="ro">av&acirc;nd un prognostic prost</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_l;&c_o;&c_k;&c_a;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;<klr tip="amb">
    (&c_m;&c_je;&c_d;.)</klr></trd>,
    <trd>&c_z;&c_l;&c_o;&c_v;&c_r;&c_je;&c_d;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">mal&iacute;gny</trd>
  <trd lng="uk">&#x437;&#x43B;&#x43E;&#x44F;&#x43A;&#x456;&#x441;&#x43D;&#x438;&#x439;</trd>
  <trdgrp lng="zh">
    <trd>&#x6076;&#x6027; <pr>&#xE8;x&#xEC;ng</pr></trd>,
    <trd>&#x60E1;&#x6027; <pr>&#xE8;x&#xEC;ng</pr></trd>
  </trdgrp>
</drv>
</art>

