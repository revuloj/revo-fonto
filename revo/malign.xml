<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: malign.xml,v 1.17 2025/10/31 12:58:50 revo Exp $">
<kap>
  <rad>malign</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="malign.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="malign.0a.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Kun detrua, vivdan&gcirc;era, prognozo:
      <ekz>
        apartaj tipoj de la homaj papilom-virusoj povas ka&ubreve;zi <tld/>ajn tumorojn
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk>Vakcino preventas kancerojn</vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        viruso <klr>[&#x2026;]</klr> de la alta <tld/>eco
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2009/009882.php"
              >Kelkaj meditoj rilate al la porka gripo</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ni faras justan militon <klr>[&#x2026;]</klr> ekstermante tiel <tld/>an
        <klr>(vd <ref cel="malic.0a">malica</ref>, <ref cel="kor.malbon0a">malbonkora</ref>)</klr>
        rason
        <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 8a, p.
        65a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_l;&c_a;&c_m;&c_y;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_z;&c_l;&c_o;&c_k;&c_a;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;</trd>
  <trd lng="ca">maligne</trd>
  <trdgrp lng="cs">
    <trd>zhoubn&yacute;</trd>,
    <trd>&scaron;kodliv&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>maligne</trd>,
    <trd>b&ouml;sartig <klr>(Medizin)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>malicious</trd>,
    <trd>malignant</trd>
  </trdgrp>
  <trd lng="fr">malin<klr> (nuisible)</klr></trd>
  <trd lng="hu">rosszindulat&uacute;<klr tip="ind"> (pl. daganat)</klr></trd>
  <trd lng="ja">&#x60AA;&#x6027;&#x306E;  <pr>&#x3042;&#x304F;&#x305B;&#x3044;&#x306E;</pr></trd>
  <trd lng="nl">kwaadaardig</trd>
  <trd lng="pl">maj&aogonek;cy z&lstroke;e rokowania</trd>
  <trd lng="pt">maligno</trd>
  <trd lng="ro">av&acirc;nd un prognostic prost</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_l;&c_o;&c_k;&c_a;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;<klr tip="amb">
    (&c_m;&c_je;&c_d;.)</klr></trd>,
    <trd>&c_z;&c_l;&c_o;&c_v;&c_r;&c_je;&c_d;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">mal&iacute;gny</trd>
  <trd lng="uk">&c_z;&c_l;&c_o;&c_ja;&c_k;&c_ib;&c_s;&c_n;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x6076;&#x6027; <pr>&egrave;x&igrave;ng</pr></trd>,
    <trd>&#x60E1;&#x6027; <pr>&egrave;x&igrave;ng</pr></trd>
  </trdgrp>
</drv>
</art>

