<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>karot</rad>/o
</kap>
<drv mrk="karot.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">KUI</uzo>
  <snc mrk="karot.0o.KUI">
    <dif>
      La man&gcirc;ebla radiko de la planto <tld/>o.
    </dif>
  </snc>
  <snc mrk="karot.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">Daucus</trd>), ofte kun unuopa nigra floro meze de la
      floraro. La oran&gcirc;kolora radiko de la kulturataj varia&jcirc;oj
      estas man&gcirc;ata kiel legomo.
    </dif>
    <bld lok="../bld/karot.jpg">
      floraro kun meza nigra floro
    </bld>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_m;&c_ib;&c_n;</trd>
  <trdgrp lng="de">
     <trd>M&ouml;hre</trd>,
     <trd>Mohrr&uuml;be</trd>,
     <trd>Gelbe R&uuml;be</trd>,
     <trd>Karotte</trd>
  </trdgrp>
  <trd lng="en">carrot</trd>
  <trd lng="es">zanahoria</trd>
  <trd lng="fr">carotte</trd>
  <trd lng="nl">wortel</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_k;&c_o;&c_v;&c_mol;</trd>
</drv>
</art>
