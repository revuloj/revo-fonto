<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">

<kap>
<rad>Antverpen</rad>/o
</kap>
<drv mrk="antverp.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="antverp.0o.urb">
    <dif>
      <ref tip="super" cel="urb.0o">Urbo</ref> en norda 
      <ref tip="malprt" cel="belg.B0ujo">Belgujo</ref>, &ccirc;e la bordo de la 
      rivero Skeldo (4&ring;24'E, 51&ring;13'N)): <ekz>Studentoj, kiuj havas pluajn ambiciojn, povas 
      da&ubreve;rigi sian studadon en la Re&gcirc;a Flandra Muzikkonservatorio en <tld/> 
      <fnt><aut>Jo HAAZEN</aut>:
      <vrk><url ref="&Monato;/005446.html">Unika muziklernejo en Belgio</url></vrk>, 
      <bib>Monato</bib>, 
      <lok>n-ro 04/1997</lok> </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="antverp.0o.prov">
    <dif>
      <ref tip="super" cel="provin.0o">Provinco</ref> en norda 
      <ref tip="malprt" cel="belg.B0ujo">Belgujo</ref> kies &ccirc;efurbo estas <tld/>o.
    </dif>
  </snc>
  
  <trd lng="de">Antwerpen</trd>
  <trd lng="en">Antwerp</trd>
  <trd lng="es">Amberes</trd>
  <trd lng="fr">Anvers</trd>
  <trd lng="it">Anversa</trd>
  <trd lng="nl">Antwerpen</trd>
  <trd lng="pl">Antwerpia</trd>
</drv>


</art>


