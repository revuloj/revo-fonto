<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">

<kap>
<rad>Antverpen</rad>/o
</kap>
<drv mrk="antverp.A0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="antverp.A0o.urb">
    <dif>
      <ref tip="super" cel="urb.0o">Urbo</ref> en la belga provinco 
      <ref tip="malprt" cel="antverp.A0o.prov">Antverpeno</ref>, en
      norda 
      Belgujo, &ccirc;e la bordo de la rivero <ref tip="vid"
cel="skeld.S0o">Skeldo</ref> 
      (4&#730;24'E, 51&#730;13'N): <ekz>studentoj, kiuj havas pluajn
ambiciojn, povas 
      da&ubreve;rigi sian studadon en la Re&gcirc;a Flandra
Muzikkonservatorio en <tld/>o 
      <fnt><aut>Jo HAAZEN</aut>:
      <vrk><url ref="&Monato;/005446.html">Unika muziklernejo en
Belgio</url></vrk>, 
      <bib>Monato</bib>, 
      <lok>n-ro 04/1997</lok> </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="antverp.A0o.prov">
    <dif>
      Unu el la kvin <ref tip="super" cel="provin.0o">provincoj</ref> de
      <ref tip="malprt" cel="flandr.F0ujo.reg">Flandrujo</ref>, en norda
      
      Belgujo, kun &ccirc;efurbo <ref tip="prt"
cel="antverp.A0o.urb">Antverpeno</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_A;&c_n;&c_t;&c_v;&c_e;&c_r;&c_p;&c_e;&c_n;</trd>
  <trd lng="bg">&c_A;&c_n;&c_t;&c_v;&c_je;&c_r;&c_p;&c_je;&c_n;</trd>
  <trd lng="ca">Anvers</trd>
  <trd lng="cs">Antverpy</trd>
  <trd lng="de">Antwerpen</trd>
  <trd lng="en">Antwerp</trd>
  <trd lng="es">Amberes</trd>
  <trd lng="fr">Anvers</trd>
  <trd lng="hu">Antwerpen</trd>
  <trd lng="it">Anversa</trd>
  <trd lng="lat">Antverpia</trd>
  <trd lng="lv">Antverpene</trd>
  <trd lng="nl">Antwerpen</trd>
  <trd lng="pl">Antwerpia</trd>
  <trd lng="pt">Antu&eacute;rpia</trd>
  <trd lng="ru">&c_A;&c_n;&c_t;&c_v;&c_je;&c_r;&c_p;&c_je;&c_n;</trd>
   </drv>


</art>


