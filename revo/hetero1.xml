<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hetero1.xml,v 1.2 2024/11/13 05:28:58 afido Exp $">
<kap>
  <rad>heteropter</rad>/oj <fnt><bib>PIV2</bib></fnt>
</kap>

<drv mrk="hetero1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Subordo el la ordo <ref tip="malprt" cel="hemipt.0oj">hemipteroj</ref>
      (<trd lng="la">Heteroptera</trd>):
      <ekz>
        la koreedoj (<nom>Coreidae</nom>) <klr>[&#8230;]</klr> apartenas al
        la hemiptera subordo de <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=424983">Koreedoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="cim.0o">cimo</ref>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_a;&c_p;&c_y;</trd>
  <trd lng="bg">&c_s;&c_malmol;&c_shch;&c_i;&c_n;&c_s;&c_k;&c_i; &c_d;&c_malmol;&c_r;&c_v;&c_je;&c_n;&c_i;&c_c;&c_i;</trd>
  <trd lng="ca">heter&ograve;pters</trd>
  <trdgrp lng="de">
    <trd>Wanzen</trd>,
    <trd>Ungleichfl&uuml;gler<klr> (malofte; indikas anka&ubreve; libelojn en Anisoptera)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Heteroptera</trd>,
    <trd>heteropterans</trd>,
    <trd>true bugs<klr> (malofte; indikas pli ofte &ccirc;iujn hemipterojn)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>h&eacute;t&eacute;ropt&egrave;res</trd>,
    <trd>punaises</trd>
  </trdgrp>
  <trd lng="hu">polosk&aacute;k</trd>
  <trdgrp lng="it">
    <trd>eterotteri</trd>,
    <trd>cimici</trd>
  </trdgrp>
  <trd lng="ja">&#12459;&#12513;&#12512;&#12471;&#20124;&#30446; <pr>&#12363;&#12417;&#12416;&#12375;&#12354;&#12418;&#12367;</pr></trd>
  <trd lng="nl">wantsen</trd>
  <trd lng="pt">heter&oacute;pteros</trd>
  <trd lng="ru">&c_k;&c_l;&c_o;&c_p;&c_y;</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_l;&c_o;&c_p;&c_i;</trd>,
    <trd>&c_s;&c_p;&c_r;&c_a;&c_v;&c_zh;&c_n;&c_ib; &c_k;&c_l;&c_o;&c_p;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="hetero1.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Apartenanta al <tld/>oj:
      <ekz>
        ili varias en grandeco de 7 &gcirc;is 45 mm, inter ili iuj el la plej grandaj <tld/>aj specioj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=424983">Koreedoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Wanzen-</trd>
  </trdgrp>
  <trd lng="ca">heter&ograve;pter</trd>
  <trd lng="ja">&#12459;&#12513;&#12512;&#12471;&#20124;&#30446;&#12398; <pr>&#12363;&#12417;&#12416;&#12375;&#12354;&#12418;&#12367;&#12398;</pr></trd>
</drv>
</art>

