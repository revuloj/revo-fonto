<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>tiar</rad>/o
</kap>
<drv mrk="tiar.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ceremonia kapvesto de la papo, ornamita per tri kronoj.
    </dif>
    <refgrp tip="sin">
      <ref cel="mitr.0o">mitro</ref>,
      <ref cel="cidar.0o">cidaro</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_y;&c_ja;&c_r;&c_a;</trd>
  <trd lng="de">Tiara</trd>
  <trdgrp lng="en">
    <trd>tiara</trd>,
    <trd>papal crown</trd>
  </trdgrp>
  <trd lng="fr">tiare</trd>
  <trd lng="hu">tiara</trd>
  <trd lng="nl">tiara</trd>
  <trd lng="pl">tiara</trd>
  <trdgrp lng="pt">
    <trd>tiara</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_i;&c_a;&c_r;&c_a;</trd>
</drv>
</art>
