<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tiar.xml,v 1.19 2018/11/09 20:10:18 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>tiar</rad>/o
</kap>
<drv mrk="tiar.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ceremonia <ref tip="super" cel="vest.kap0o">kapvesto</ref>
      de la papo, ornamita per tri kronoj:
      <ekz>
        el la ondoj, fi&scirc;o kara, venu al la bordo mara, Ilzo min &ccirc;i tien sendas, la <tld/>on de vi mendas
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fi&scirc;kaptisto kaj lia edzino</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi demandas vian ekscelencon, trankvile ripetis la faraono,
        je kia rajto sur via <tld/>o estas la re&gcirc;a serpento
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>?
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="mitr.0o">mitro</ref>,
      <ref cel="cidar.0o">cidaro</ref>.
    </refgrp>
    <bld
      lok="&WCU;/thumb/6/6c/Tiara.png/182px-Tiara.png"
      prm="PD"
      >papa <tld/>o
        <fnt>
          <aut>Meyers Konversations-Lexikon, 4a eldono, 1888&ndash;1890</aut>
          <lok><url ref="&WCW;/File:Tiara.png"
              >&WCW;/File:Tiara.png</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_t;&c_y;&c_ja;&c_r;&c_a;</trd>
  <trd lng="bg">&c_t;&c_i;&c_a;&c_r;&c_a;</trd>
  <trd lng="ca">tiara</trd>
  <trdgrp lng="cs">
    <trd>pape&zcaron;sk&aacute; koruna</trd>,
    <trd>tiara</trd>
  </trdgrp>
  <trd lng="de">Tiara</trd>
  <trdgrp lng="en">
    <trd>tiara</trd>,
    <trd>papal crown</trd>
  </trdgrp>
  <trd lng="fr">tiare</trd>
  <trd lng="hu">tiara</trd>
  <trd lng="nl">tiara</trd>
  <trd lng="pl">tiara</trd>
  <trdgrp lng="pt">
    <trd>tiara</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_i;&c_a;&c_r;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>p&aacute;pe&zcaron;sk&aacute; koruna</trd>,
    <trd>tiara</trd>
  </trdgrp>
</drv>
</art>
