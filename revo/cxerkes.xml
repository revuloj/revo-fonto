<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cxerkes.xml,v 1.13 2023/10/31 13:37:03 revo Exp $">
<kap>
  <rad>&ccirc;erkes</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="cxerkes.0o">
  <kap><tld/>o</kap>
  <snc mrk="cxerkes.0o.ETN">
    <dif>
      Ano de nordka&ubreve;kaza popolo:
      <ekz>
        <tld/>oj tramar&scirc;is, kantante la&ubreve;ritme rabkantojn
        <fnt><bib>VivZam</bib>, <lok>&ccirc;apitro 12a, p. 120a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_r;&c_k;&c_je;&c_s;</trd>
  <trd lng="de">Tscherkesse</trd>
  <trdgrp lng="en"><trd>Circassian</trd>, <trd>Adyghe</trd></trdgrp>
  <trdgrp lng="fr">
    <trd>Circassien<klr> (subst.)</klr></trd>,
    <trd>Tcherkesse<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="it">circasso<klr> (sost.)</klr></trd>
  <trd lng="ja">&#x30C1;&#x30A7;&#x30EB;&#x30B1;&#x30B9;&#x4EBA;  <pr>&#x30C1;&#x30A7;&#x30EB;&#x30B1;&#x30B9;&#x3058;&#x3093;</pr></trd>
  <trd lng="pl">Czerkies</trd>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_k;&c_je;&c_s;</trd>
  <trd lng="uk">&c_ch;&c_je;&c_r;&c_k;&c_je;&c_s;</trd>
</drv>

<drv mrk="cxerkes.0a">
  <kap><tld/>a</kap>
  <snc mrk="cxerkes.0a.ETN">
    <dif>
      Rilata al la <tld/>oj:
      <ekz>
        la generalo <nom>Jermolov</nom>, en <tld/>a mantelo minace sulkigis la
        brovojn
        <fnt><bib>PatrojFiloj</bib>, <lok>&ccirc;apitro 8a, p.
        38a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_r;&c_k;&c_je;&c_s;&c_k;&c_ib;</trd>
  <trd lng="de">tscherkessisch</trd>
  <trd lng="en">Circassian</trd>
  <trdgrp lng="fr">
    <trd>circassien<klr> (adj.)</klr></trd>,
    <trd>tcherkesse<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="it">circasso<klr> (agg.)</klr></trd>
  <trd lng="pl">czerkieski</trd>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_k;&c_je;&c_s;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="zh">&#x5361;&#x5DF4;&#x5C14;&#x8FBE;&#x8BED; <pr>k&#x1CE;b&#x101;&#x11B;rd&#xE1;y&#x1D4;</pr></trd>
</drv>
</art>

