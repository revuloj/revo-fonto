<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>Laert</rad>/o
</kap>
<drv mrk="laert.0o">
  <kap><tld/>o</kap>
  <snc mrk="laert.0o.mit">
  <uzo tip="fak">MIT</uzo>
    <dif>
    Patro de <ref tip="vid" cel="odise.0o.PERS">Odiseo</ref>,
    partopreninta
    en la ekspedicio de la
<ref tip="vid" cel="argonauxt.0j">Argona&ubreve;toj</ref> kaj al la unua milito
kontra&ubreve; Trojo.
    </dif>
  </snc>

  <snc mrk="laert.0o.nomo">
  <uzo tip="fak">NOM</uzo>
    <dif>
Grekdevena vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref>.
    </dif>
  </snc>
  <trd lng="bg">&c_L;&c_a;&c_je;&c_r;&c_t;</trd>
  <trd lng="ca">Laertes</trd>
  <trdgrp lng="de">
    <trd>Laertes</trd>,
    <trd>La&euml;rtes</trd>
  </trdgrp>
  <trd lng="en">Laertes</trd>
  <trd lng="it">Laerte</trd>
</drv>
</art>

