</drv>

<drv mrk="kovr.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <snc>
    <dif>
      Envolve <tld/>i:
      <ekz>
        li dis&scirc;iris siajn vestojn kaj &ccirc;irka&ubreve;<tld/>is
        sin per sako
        <fnt><bib>MT</bib><lok>&Jes; 37:1</lok></fnt>;
      </ekz>
      <ekz>
        la kaptitan fi&scirc;on ili tuj &ccirc;irka&ubreve;<tld/>as per malseka argilo kaj ka&scirc;as &gcirc;in en
        glaciejo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i malfermis malgrandan ligpaka&jcirc;on, elprenis el &gcirc;i ian objekton, &ccirc;irka&ubreve;<tld/>is
        &gcirc;in per la tuko kaj eliris
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi fari&gcirc;os pala kaj malvarma, &gcirc;is via ha&ubreve;to &ccirc;irka&ubreve;<tld/>os nur skeleton
        <fnt>
          <bib>Fab2</bib>
          <lok>Ru&gcirc;aj &scirc;uoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu el la potencaj re&gcirc;oj de la malproksima
        antikveco, &ccirc;irka&ubreve;ita de granda lukso,
        ku&scirc;as en mumia &ccirc;irka&ubreve;<tld/>o
        <fnt><bib>Fab4</bib>, <lok>Filino de la mar&ccirc;a
        re&gcirc;o</lok></fnt>;
      </ekz><ekz>
        tiam defalis la rana &ccirc;irka&ubreve;<tld/>o,
        &scirc;i estis la juna belulino
        <fnt><bib>Fab4</bib>, <lok>Filino de la mar&ccirc;a
        re&gcirc;o</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_g;&c_a;&c_r;&c_n;&c_u;&c_c;&c_mol;</trd>,
      <trd>&c_u;&c_h;&c_u;&c_t;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="ca">recobrir</trd>
    <trdgrp lng="de">
      <trd>einwickeln</trd>,
      <trd>umwickeln</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>envelop</trd>,
      <trd>wrap up</trd>
    </trdgrp>
    <trd lng="es">recubrir</trd>
    <trdgrp lng="fr">
      <trd>envelopper</trd>,
      <trd>emmailloter</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>bebugyol&aacute;l</trd>,
      <trd>beburkol</trd>,
      <trd>beg&ouml;ngy&ouml;l</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>owija&cacute;</trd>,
      <trd>zawija&cacute;</trd>
    </trdgrp>   
    <trdgrp lng="ro">
