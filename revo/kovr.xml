    <trd lng="hu">fed&eacute;s</trd>
    <trd lng="pl">pokrycie<klr tip="ind"> (zbioru
    zbiorami)</klr></trd>
    <trd lng="ru">&c_p;&c_o;&c_k;&c_r;&c_y;&c_t;&c_i;&c_je;<klr tip="ind"> (&c_p;&c_o;&c_k;&c_r;&c_y;&c_v;&c_a;&c_ju;&c_shch;&c_je;&c_je;
        &c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </snc>
  <trdgrp lng="cs">
