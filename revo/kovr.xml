    <drv mrk="kovr.0ilo">
      <kap><ofc>*</ofc><tld/>ilo</kap>
      <snc mrk="kovr.0ilo.KOMUNE">
        <dif>
          &Ccirc;io, kion oni uzas por <tld/>i ion ajn, kaj precipe:
        </dif>
        <subsnc mrk="kovr.0ilo.MEBL">
          <dif>
            Movebla parto, kiun oni metas a&ubreve; mallevas sur aperturon de
            vazo, kesto, kofro ktp, por &gcirc;in fermi:
            <ekz>
              &ccirc;iu nefermita vazo, sur kiu ne estas <tld/>ilo kun
              &scirc;nureto, estas malpura
              <fnt><bib>MT</bib>, <lok>&Nom; 19:15</lok></fnt>;
            </ekz><ekz>
              en tiu momento oni forportis la &ccirc;erkon [...], kaj oni
              albatis
              la
              <tld/>ilon
              <fnt><bib>Fab4</bib>, <lok>Infano en la tombo</lok></fnt>.
            </ekz>
          </dif>
          <trdgrp lng="ca">
            <trd>tapa</trd>,
            <trd>coberta</trd>
          </trdgrp>
          <trd lng="de">Deckel</trd>
          <trdgrp lng="es">
            <trd>tapa</trd>,
            <trd>cubierta</trd>
          </trdgrp>
          <trd lng="en"><klr>(pot)</klr> lid</trd>
          <trd lng="fr">couvercle</trd>
          <trd lng="hu">fed&eacute;l</trd>
          <trd lng="ru">&c_k;&c_r;&c_y;&c_sh;&c_k;&c_a;</trd>
        </subsnc>
        <subsnc mrk="kovr.0ilo.HEJM">
          <dif>
            Lana a&ubreve; kotona teksa&jcirc;o, kiun oni metas sur liton por
            konservi
            varmon:
            <ekz>
              etendu la piedojn kiom via <tld/>ilo permesas<fnt>Z</fnt>;
            </ekz><ekz>
              la ne&gcirc;a nubo pretigos al vi molan liton, &gcirc;i metos tutan
              litotukon, molan lit<tld/>ilon &ccirc;irka&ubreve; viajn piedojn
              <fnt><bib>Fab4</bib>, <lok>Lasta son&gcirc;o de maljuna
                  kverko</lok></fnt>.
            </ekz>
          </dif>
          <refgrp tip="vid">
            <ref cel="tapisx.0o">tapi&scirc;o</ref>.
          </refgrp>
          <trd lng="ca">manta</trd>
          <trdgrp lng="de">
            <trd>Decke</trd>,
            <trd>Bettdecke</trd>
          </trdgrp>
          <trdgrp lng="en">
            <trd>(bed) cover</trd>,
            <trd>blanket</trd>,
            <trd>counterpane</trd>
         </trdgrp>
         <trdgrp lng="es">
            <trd>manta</trd>,
            <trd>colcha</trd>,
            <trd>cobertor</trd>
          </trdgrp>
          <trd lng="fr">couverture <klr>(literie)</klr></trd>
          <trdgrp lng="hu">
            <trd>takar&oacute;</trd>,
            <trd>&aacute;gytakar&oacute;</trd>
          </trdgrp>
          <trdgrp lng="ru">
            <trd>&c_p;&c_o;&c_k;&c_r;&c_y;&c_v;&c_a;&c_l;&c_o;</trd>,
            <trd>&c_o;&c_d;&c_je;&c_ja;&c_l;&c_o;</trd>
          </trdgrp>
        </subsnc>
        <subsnc mrk="kovr.0ilo.TIP">
          <uzo tip="fak">TIP</uzo>
          <dif>
            Papera, kartona, leda ekstera parto de libro:
            <ekz>
              <tld/>ilo de libro.
            </ekz>
          </dif>
          <trdgrp lng="ca">
            <trd>tapes <klr>(d'un llibre)</klr></trd>,
            <trd>coberta</trd>
          </trdgrp>
          <trdgrp lng="de">
            <trd>Einband</trd>,
            <trd>Umschlag</trd>
          </trdgrp>
          <trd lng="en"><klr>(book)</klr> cover</trd>
          <trd lng="fr">couverture <klr>(de livre)</klr></trd>
          <trdgrp lng="es">
            <trd>portada</trd>,
            <trd>cubierta</trd>
          </trdgrp>
          <trdgrp lng="hu">
            <trd>bor&iacute;t&oacute;lap</trd>,
            <trd>k&ouml;nyvbor&iacute;t&oacute;</trd>
          </trdgrp>
          <trd lng="ru">&c_o;&c_b;&c_l;&c_o;&c_zh;&c_k;&c_a;</trd>
        </subsnc>
        <subsnc mrk="kovr.0ilo.ARKI">
          <uzo tip="fak">ARKI</uzo>
          <dif>
            Panelo, kiu estas adaptita ekstere a&ubreve; interne kaj povas
            malfermi&gcirc;i kontra&ubreve; la fenestro;
            <ref tip="vid" cel="sxutr.0o.ARKI">&scirc;utro</ref>.
          </dif>
          <trdgrp lng="de">
            <trd>Laden</trd>,
            <trd>Fensterladen</trd>
          </trdgrp>
          <trd lng="en"><klr>(window)</klr> shutter</trd>
          <trd lng="fr">volet <klr>(de fen&ecirc;tre)</klr></trd>
          <trdgrp lng="hu">
            <trd>ablakt&aacute;bla</trd>,
            <trd>spaletta</trd>
          </trdgrp>
          <trd lng="ru">&c_s;&c_t;&c_a;&c_v;&c_je;&c_n;&c_mol;</trd>
        </subsnc>
        <subsnc mrk="kovr.0ilo.FIG">
          <uzo tip="stl">FIG</uzo>
          <dif>
            &Ccirc;io, kio &scirc;irmas, bare malhelpas:
            <ekz>
              defaligi la <tld/>ilon de la okuloj<fnt>Z</fnt>;
            </ekz><ekz>
              mi neniam <tld/>ilon <klr>(&scirc;irmilon, pretekston)</klr>
              faris el la pastra rajto<fnt>Z</fnt>.
            </ekz>
          </dif>
          <adm>
            Mi aldonis la PIV-an difinon, sed trovas, ke nek &gcirc;i, nek
            la ekzemploj bone precizigas la sencon.<aut>MB</aut>
          </adm>
          <trdgrp lng="ru">
            <trd>&c_p;&c_je;&c_l;&c_je;&c_n;&c_a;</trd>,
            <trd>&c_p;&c_o;&c_k;&c_r;&c_y;&c_v;&c_a;&c_l;&c_o;</trd>,
            <trd>&c_p;&c_r;&c_i;&c_k;&c_r;&c_y;&c_t;&c_i;&c_je;</trd>
          </trdgrp>
        </subsnc>
      </snc>
      <rim>
        <em><tld/>a&jcirc;o</em> estas pli &gcirc;enerala ol <em><tld/>ilo</em>, kiu
        montras apartan ilon a&ubreve; rimedon por difinita celo..
      </rim>
      <trd lng="be">&c_p;&c_o;&c_k;&c_r;&c_y;&c_w;&c_k;&c_a;</trd>
      <trd lng="bo">&#xF55;&#xF7C;&#xF0B;&#xF41;&#xF7A;&#xF56;&#xF0B;</trd>
  <trdgrp lng="cs">
        <trd>desky knihy</trd>,
        <trd>kryt</trd>,
        <trd>poklop</trd>,
        <trd>pokr&yacute;vka</trd>
      </trdgrp>     
      <trdgrp lng="de">
        <trd>Abdeckung</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>wieko</trd>,
        <trd>ok&lstroke;adka</trd>,
        <trd>pokrowiec</trd>,
        <trd>poszwa</trd>,
        <trd>pokrywka</trd>
      </trdgrp>   
      <trdgrp lng="ro">
        <trd>capac</trd>,
        <trd>&icirc;nveli&#x219;</trd>,
        <trd>copertin&abreve; de duvet</trd>,
        <trd>caz</trd>,
        <trd>p&abreve;tur&abreve;</trd>,
        <trd>fe&#x21B;e de pern&abreve;</trd>
      </trdgrp>
      <trd lng="ru">&c_p;&c_o;&c_k;&c_r;&c_y;&c_t;&c_i;&c_je;</trd>
      <trdgrp lng="sk">
        <trd>deka</trd>,
        <trd>pokr&yacute;vka</trd>,
        <trd>prikr&yacute;vka</trd>,
        <trd>veko</trd>,
        <trd>vrchn&aacute;k</trd>
      </trdgrp>
</drv>

    <drv mrk="kovr.cxirkaux0i">
      <kap>&ccirc;irka&ubreve;<tld/>i</kap>
