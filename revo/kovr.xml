    <drv mrk="kovr.0o">
      <kap><tld/>o</kap>
      <snc mrk="kovr.0o.komune">
        <dif>
          Io, kio <tld/>as:
          <ekz>
            sur la ne&gcirc;a <tld/>o brilis grandaj diamantoj
            <fnt><bib>Fab4</bib>, <lok>Ne&gcirc;ulo</lok></fnt>;
          </ekz><ekz>
            &gcirc;i estas lia sola <tld/>o, &gcirc;i estas la vesto por lia korpo
            <fnt><bib>MT</bib>, <lok>&Eli; 22:27</lok></fnt>;
          </ekz><ekz>
            faru tapi&scirc;ojn el kapra lano kiel <tld/>on super la
            tabernaklo
            <fnt><bib>MT</bib>, <lok>&Eli; 26:7</lok></fnt>;
          </ekz><ekz>
            sub ombrori&ccirc;aj arboj &gcirc;i ku&scirc;as, sub <tld/>o de
            kanoj kaj en &scirc;limo
            <fnt><bib>MT</bib>, <lok>&Ijo; 40:21</lok></fnt>;
          </ekz><ekz>
            &scirc;i faras al si <tld/>ojn: delikata tolo kaj purpuro estas
            &scirc;iaj vestoj
            <fnt><bib>MT</bib>, <lok>&Sen; 31:22</lok></fnt>;
          </ekz><ekz>
            &scirc;i vidis pentritajn virojn sur la muro [...], kun longaj
            kap<tld/>oj sur siaj kapoj
            <fnt><bib>MT</bib>, <lok>&Jeh; 23:15</lok></fnt>.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="tavol.0o">tavolo</ref>,
          <ref cel="teg.0o">tego</ref>,
          <ref cel="tegmen.0o">tegmento</ref>,
          <ref cel="stern.0ajxo">sterna&jcirc;o</ref>,
          <ref cel="vest.0o">vesto</ref>.
        </refgrp>
        <trd lng="ca">coberta</trd>
        <trdgrp lng="de">
          <trd>Decke</trd>,
          <trd>&Uuml;berzug</trd>
        </trdgrp>
        <trdgrp lng="en">
          <trd>cover</trd>,
          <trd>covering</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>couverture <klr>(ce qui couvre, ce dont on se
              couvre)</klr></trd>,
          <trd>couvert <klr>(des arbres)</klr></trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>takar&aacute;s</trd>,
          <trd>fed&eacute;s</trd>
        </trdgrp>
        <trd lng="pl">pokrycie</trd>
        <trdgrp lng="ru">
          <trd>&c_p;&c_o;&c_k;&c_r;&c_y;&c_t;&c_i;&c_je;</trd>,
          <trd>&c_p;&c_o;&c_k;&c_r;&c_o;&c_v;</trd>,
          <trd>&c_u;&c_b;&c_o;&c_r;</trd>
        </trdgrp>
      </snc>
      <snc mrk="kovr.0o.MAT">
        <uzo tip="fak">MAT</uzo>
        <fnt><bib>EKV</bib><lok>&FE;238</lok></fnt>
        <dif>
          <klr>(de <ref cel="ar.0o.MAT">aro</ref>
          <frm><g>E</g></frm>)</klr>
          <ref tip="super" cel="famili.0o.bildigo">Familio</ref> de
          subaroj, kies <ref cel="kun.0ajxo.aro">kuna&jcirc;o</ref>
          inkluzivas <frm><g>E</g></frm>.
        </dif>
        <ref tip="sub" cel="part.dis0igo.MAT">dispartigo;</ref>
        <ref tip="vid" cel="kompak.kvazaux0a.MAT">kvaza&ubreve;kompakta.</ref>
        <trd lng="de">&Uuml;berdeckung</trd>
        <trd lng="fr">recouvrement<klr tip="ind"> (math.)</klr></trd>
        <trd lng="hu">fed&eacute;s</trd>
