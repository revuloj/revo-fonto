<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>talar</rad>/o
    </kap>

<drv mrk="talar1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Malnova <ref tip="lst" cel="unu.mon0o">monunuo</ref>:
      <ekz>
        jen estas 50 <tld/>oj, iru en la vastan mondon kaj al neniu diru, de kie vi
        venas kaj kiu estas via patro, &ccirc;ar mi hontas pro vi
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fabelo pri iu, kiu migris por ekkoni timon</lok>
        </fnt>;
      </ekz>
      <ekz>
        Steloj <tld/>oj <klr>(fabelo)</klr>
        <fnt>
          <bib>ElektFab</bib>
          <lok>Steloj talaroj</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="dolar.0o">dolaro</ref>
    <trd lng="de">Taler</trd>
  </snc>
  <rim>
    <ref tip="hom" cel="talar.0o">Talaro</ref>
    estas anka&ubreve; vesto.
  </rim>
</drv>

  </art>

<!--
$Log$
-->
</vortaro>