<vortaro>
<art mrk="$Id: fung.xml,v 1.48 2023/11/10 06:08:01 revo Exp $">
<kap><ofc>*</ofc>
<rad>fung</rad>/o</kap>

<drv mrk="fung.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fung.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      Iu el <ref tip="malprt" cel="fung.0oj"><tld/>oj</ref>,
      viva&jcirc;o plur&ccirc;ela, nutri&gcirc;anta de
      organika materialo kaj reprodukti&gcirc;anta plej
      ofte per sporoj:
      <ekz>
        li estis evoluiginta specion de tritiko, kiu rezistis al la <tld/>o &#x201A;nigra rusto&#x2018;
        <fnt>
          <bib>Monato</bib>
          <aut>Joyce Bunting</aut>
          <vrk><url ref="&Monato;/2013/010846.php">Taktiko por noveca tritiko</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        la putradon ka&ubreve;zis la <tld/>o <nom>Neofabraea alba</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>D. E. Rogers</aut>
          <vrk><url ref="&Monato;/2014/011160.php">Kiam nutra&jcirc;o ka&scirc;as putra&jcirc;on</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="sxim.0fungo">&scirc;imfungo</ref>
    <trd lng="en">fungus</trd>
    <trd lng="es">hongo</trd>
  </snc>
  <snc mrk="fung.0o.fruktokorpo">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Fruktokorpo de <ref tip="malprt" cel="fung.0o.BIO"><tld/>o
      <sncref ref="fung.0o.BIO"/></ref>:            
      <ekz>
        rompebla kiel <ind>fajro<tld/>o</ind>
        <klr>(<ref cel="tindr.0o">tindro</ref>)</klr>
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>;
      </ekz>
      <ekz>
        por la <tld/>oj la kamparaninoj ricevis kvardek du kuprajn kopekojn
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi mense vidas &scirc;in miksi siajn venenajn <tld/>ojn
        <fnt>
          <bib>ChC</bib>
        </fnt>;
      </ekz>
      <ekz>
        post la <tld/>oj mi ankora&ubreve; trinkos
        <fnt>
          <bib>IKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        novaj artaj lingvoj elkreskadas kiel <tld/>oj post pluvo
        <fnt>
          <bib>EE</bib>
        </fnt>.
      </ekz>          
    </dif>
    <trdgrp lng="en">
      <trd>mushroom</trd>,
      <trd>toadstool</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>seta</trd>,
      <trd>champi&ntilde;&oacute;n</trd>
    </trdgrp>
  </snc>
  <lstref lst="voko:fungoj"><tld/>oj</lstref>
  <trd lng="be">&c_g;&c_r;&c_y;&c_b;</trd>
  <trd lng="bg">&c_g;&c_mol;&c_b;&c_a;</trd>
  <trd lng="bo">&t_sha;&t_tsheg;&t_ma;&t_naro;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>houba</trd>,
    <trd>pl&iacute;se&ncaron;</trd>
  </trdgrp>
  <trd lng="de">Pilz</trd>
  <trdgrp lng="es">
    <trd>hongo</trd>,
    <trd>seta</trd>,
    <trd>champi&ntilde;&oacute;n</trd>
  </trdgrp>
  <trd lng="fr">champignon</trd>
  <trd lng="hu">gomba</trd>
  <trdgrp lng="id">
    <trd>cendawan</trd>,
    <trd>jamur</trd>
  </trdgrp>
  <trd lng="ja">&#x30AD;&#x30CE;&#x30B3;</trd>
  <trd lng="nl">zwam</trd>
  <trd lng="pl">grzyb</trd>
  <trdgrp lng="pt">
    <trd>fungo</trd>,
    <trd>cogumelo</trd>,
    <trd>tortulho</trd>
  </trdgrp>
  <trd lng="ro">ciuperc&abreve;</trd>
  <trd lng="ru">&c_g;&c_r;&c_i;&c_b;</trd>
  <trdgrp lng="sk">
    <trd>huba</trd>,
    <trd>plese&ncaron;</trd>
  </trdgrp>
  <trd lng="tok">kili</trd>
  <trd lng="uk">&c_g;&c_r;&c_i;&c_b;</trd>
  <trdgrp lng="zh">
    <trd>&#x82D3; <pr>l&#xED;ng</pr></trd>,
    <trd>&#x83CC;&#x7C7B; <pr>j&#xF9;nl&#xE8;i</pr></trd>,
    <trd>&#x83CC; <pr>j&#x16B;n</pr></trd>,
    <trd>&#x8611;&#x83C7; <pr>m&#xF3;gu</pr></trd>,
    <trd>&#x8611; <pr>m&#xF3;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fung.0oj">
  <kap><tld/>oj</kap>
  <tezrad fak="BOT"/>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Unu el la <ref tip="lst" cel="regn.0o.BIO" lst="voko:biologiaj_regnoj">regnoj</ref>
      de la viva&jcirc;oj (<trd lng="la">Fungi</trd>):
      <ekz>
        &ccirc;iuj &ccirc;elhavaj organismoj (bakterioj, ameboj, <tld/>oj, gistoj, plantoj kaj
        animaloj) uzas DNA-on por konservi la genetikan informon
       <fnt>
          <aut>Ste&ccirc;jo Norvell</aut>
          <vrk><url ref="&Monato;/2000/007157.html">Riboj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/11, p. 16-17</lok>
          </fnt>.
      </ekz>      
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_y;&c_b;&c_y; <klr tip="amb">(&c_c;&c_a;&c_r;&c_s;&c_t;&c_v;&c_a;)</klr></trd>
  <trd lng="cs">houby <klr>(&rcaron;&iacute;&scaron;e)</klr></trd>
  <trd lng="de">Pilze</trd>
  <trd lng="en">fungi</trd>
  <trdgrp lng="es">
    <trd>hongos</trd>,
    <trd>setas</trd>,
    <trd>champi&ntilde;ones</trd>
  </trdgrp>
  <trd lng="fr">champignons</trd>
  <trd lng="hu">gomb&aacute;k</trd>
  <trd lng="id">fungi</trd>
  <trd lng="ja">&#x83CC;&#x985E;  <pr>&#x304D;&#x3093;&#x308B;&#x3044;</pr></trd>
  <trd lng="nl">zwammen</trd>
  <trd lng="pl">grzyby</trd>
  <trd lng="ru">&c_g;&c_r;&c_i;&c_b;&c_y;</trd>
  <trd lng="sk">huby</trd>
  <trd lng="uk">&c_n;&c_je;&c_z;&c_a;&c_v;&c_je;&c_r;&c_sh;&c_je;&c_n;&c_ib; &c_g;&c_r;&c_i;&c_b;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x8548;&#x7C7B; <pr>x&#xF9;nl&#xE8;i</pr></trd>,
    <trd>&#x771F;&#x83CC; <pr>zh&#x113;nj&#x16B;n</pr></trd>,
    <trd>&#x9709; <pr>m&#xE9;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fung.gisto0o">
  <kap>
    gisto<tld/>o
  </kap>
  <snc>
    <dif>
      Unu&ccirc;ela sakfungo, plimulti&gcirc;anta per bur&gcirc;onado a&ubreve; divido
      kaj disiganta sukeron en alkoholon kaj karbondioksidon;
      <ref tip="sin" cel="gist.0o.BAK">gisto<sncref/></ref>:
      <ekz>
        froma&gcirc;o neniam estu deponata &ccirc;e pano, &ccirc;ar gisto<tld/>oj infektas kaj
        malbongustigas la froma&gcirc;on
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19287">Froma&gcirc;o</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Hefepilz</trd>
  <trd lng="en">yeast</trd>
  <trd lng="fr">levure</trd>
  <trd lng="ja">&#x9175;&#x6BCD;&#x83CC;  <pr>&#x3053;&#x3046;&#x307C;&#x304D;&#x3093;</pr></trd>
  <trd lng="zh">&#x9175;&#x6BCD; <pr>xi&#xE0;om&#x1D4;</pr></trd>
</drv>









<drv mrk="fung.klab0oj">
  <kap>klab<tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Filumo de la <ref tip="super" cel="fung.0oj"><tld/>oj</ref>
      (<trd lng="la">Basidiomycota</trd>), kiuj produktas po kvar
      sporojn sur klabformaj sporingoj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_z;&c_y;&c_d;&c_y;&c_ja;&c_l;&c_mol;&c_n;&c_y;&c_ja; &c_g;&c_r;&c_y;&c_b;&c_y;</trd>,
    <trd>&c_b;&c_a;&c_z;&c_y;&c_d;&c_y;&c_ja;&c_m;&c_ib;&c_c;&c_e;&c_t;&c_y;</trd>
  </trdgrp>
  <trd lng="de">Basidienpilze</trd>
  <trd lng="en">club fungi</trd>
  <trd lng="es">basidiomycota</trd>
  <trd lng="fr">basidiomyc&egrave;tes</trd>
  <trd lng="nl">steeltjeszwammen</trd>
  <trd lng="hu">baz&iacute;diumos <ind>gomb&aacute;k</ind></trd>
  <trd lng="pl">grzyby podstawkowe</trd>
  <trd lng="zh">&#x62C5;&#x5B50;&#x83CC;&#x95E8; <pr>d&#xE0;nz&#x1D0;j&#xF9;nm&#xE9;n</pr></trd>
</drv>

<drv mrk="fung.lamen0o">
  <kap>lamen<tld/>o</kap>
  <snc>
    <dif>
      Fungo, kies ombrelforma fruktokorpo de
      sube prezentas multajn alcentrajn lamenojn.
    </dif>
  </snc>
  <ref tip="malprt" cel="agarik1.0oj">agarikacoj</ref>
  <trd lng="be">&c_p;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_n;&c_ib;&c_s;&c_t;&c_y; <ind>&c_g;&c_r;&c_y;&c_b;</ind></trd>
  <trd lng="de">Bl&auml;tterpilz</trd>
  <trd lng="es">ag&aacute;rico</trd>
  <trd lng="fr">agaric</trd>
  <trd lng="ja">&#x30DE;&#x30C3;&#x30B7;&#x30E5;&#x30EB;&#x30FC;&#x30E0;&#x306A;&#x3069;&#x30CF;&#x30E9;&#x30BF;&#x30B1;&#x79D1;&#x306E;&#x30AD;&#x30CE;&#x30B3;&#x306E;&#x7DCF;&#x79F0;  <pr>&#x307E;&#x3063;&#x3057;&#x3085;&#x308B;&#x30FC;&#x3080;&#x306A;&#x3069;&#x30CF;&#x30E9;&#x30BF;&#x30B1;&#x304B;&#x306E;&#x304D;&#x306E;&#x3053;&#x306E;&#x305D;&#x3046;&#x3057;&#x3087;&#x3046;</pr></trd>
  <trd lng="nl">lamellenzwam</trd>
  <trd lng="hu">lemezes <ind>gomba</ind></trd>
  <trd lng="pl">grzyb blaszkowy</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_s;&c_t;&c_i;&c_n;&c_ch;&c_a;&c_t;&c_y;&c_j; &c_g;&c_r;&c_i;&c_b;</trd>
  <trd lng="uk">&c_p;&c_l;&c_a;&c_s;&c_t;&c_i;&c_n;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_j; &c_g;&c_r;&c_i;&c_b;</trd>
  <trd lng="zh">&#x4F1E;&#x83CC;&#x76EE; <pr>s&#x1CE;nj&#x16B;nm&#xF9;</pr></trd>
</drv>

<drv mrk="fung.musx0o">
  <kap>mu&scirc;<tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Venena <ref tip="lst" cel="fung.0o.fruktokorpo" lst="voko:fungoj">
      <tld/>o</ref> kun ru&gcirc;a &ccirc;apo, blanke makulita;
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:botanikaj_specioj">specio</ref>
       de <ref tip="super" cel="amanit.0o">amanito</ref>
      (<trd lng="la">Amanita muscaria</trd>):
      <ekz>
        a&ubreve;dinte pri la morta dan&gcirc;ero de mu&scirc;<tld/>oj, <klr>[li]</klr>
        decidis mem elprovi, &ccirc;u tio ne estas nur kla&ccirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2010/010185.php">Malbonan apetiton</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>      
    </dif>
  </snc>
  <bld lok="../bld/musxfung.jpg">
    fruktokorpo de mu&scirc;<tld/>o
  </bld>
  <trd lng="be">&c_m;&c_u;&c_h;&c_a;&c_m;&c_o;&c_r;</trd>
  <trd lng="de">Fliegenpilz</trd>
  <trdgrp lng="es">
    <trd>amanita muscaria</trd>,
    <trd>amanita matamoscas</trd>,
    <trd>falsa oronja</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>amanite muscarine</trd>,
    <trd>amanite tue-mouches</trd>
  </trdgrp>
  <trd lng="ja">&#x30D9;&#x30CB;&#x30C6;&#x30F3;&#x30B0;&#x30BF;&#x30B1;</trd>
  <trd lng="nl">vliegenzwam</trd>
  <trd lng="hu">l&eacute;gy&ouml;l&odblac; gal&oacute;ca</trd>
  <trd lng="pl">muchomor czerwony</trd>
  <trd lng="ru">&c_m;&c_u;&c_h;&c_o;&c_m;&c_o;&c_r;</trd>
  <trd lng="uk">&c_m;&c_u;&c_h;&c_o;&c_m;&c_o;&c_r;</trd>
</drv>

<drv mrk="fung.sak0oj">
  <kap>sak<tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Filumo de la <ref tip="super" cel="fung.0oj"><tld/>oj</ref>
      (<trd lng="la">Ascomycota</trd>), kiuj produktas po kvar
      a&ubreve; pli ofte ok sporojn en sakformaj sporujoj.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_o;&c_r;&c_b;&c_a;&c_ch;&c_k;&c_a;&c_v;&c_y;&c_ja; <ind>&c_g;&c_r;&c_y;&c_b;&c_y;</ind></trd>
  <trd lng="de">Schlauchpilze</trd>
  <trd lng="en">sac fungi</trd>
  <trd lng="es">ascomicetos</trd>
  <trd lng="fr">ascomyc&egrave;tes</trd>
  <trd lng="nl">zakjeszwammen</trd>
  <trd lng="hu">t&ouml;ml&odblac;s gomb&aacute;k</trd>
  <trd lng="pl">grzyby workowe</trd>
  <trd lng="ru">&c_s;&c_u;&c_m;&c_ch;&c_a;&c_t;&c_y;&c_je; &c_g;&c_r;&c_i;&c_b;&c_y;</trd>
  <trd lng="zh">&#x5B50;&#x56CA;&#x83CC;&#x9580; <pr>z&#x1D0;n&#xE1;ngj&#xF9;nm&#xE9;n</pr></trd>
</drv>

<drv mrk="fung.tub0o">
  <kap>tub<tld/>o</kap>
  <snc>
    <dif>
      Fungo, kies ombrelforma fruktokorpo de
      sube prezentas multajn tubformajn truojn.
    </dif>
  </snc>
  <ref tip="malprt" cel="boleta.0oj">boletacoj</ref>
  <trd lng="be">&c_t;&c_r;&c_u;&c_b;&c_ch;&c_a;&c_t;&c_y; <ind>&c_g;&c_r;&c_y;&c_b;</ind></trd>
  <trd lng="cs">h&rcaron;&iacute;bek</trd>
  <trd lng="de">R&ouml;hrling</trd>
  <trd lng="en">bolete</trd>
  <trd lng="es">boletus</trd>
  <trd lng="nl">boleet</trd>
  <trd lng="hu">cs&ouml;ves gomba</trd>
  <trd lng="pl">grzyb rurkowy</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_u;&c_b;&c_ch;&c_a;&c_t;&c_i;&c_j;/&c_g;&c_u;&c_b;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_j; &c_g;&c_r;&c_i;&c_b;</trd>,
    <trd>&c_b;&c_o;&c_l;&c_je;&c_t;</trd>
  </trdgrp>
  <trd lng="zh">&#x725B;&#x809D;&#x83CC;&#x79D1; <pr>ni&#xFA;g&#x101;nj&#x16B;nk&#x113;</pr></trd>
</drv>
