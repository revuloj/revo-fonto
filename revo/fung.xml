<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fung</rad>/o
</kap>
<drv mrk="fung.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Iu el <ref tip="malprt" cel="fung.0oj">fungoj</ref>:
      <ekz>
        &scirc;imo estas <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">houba</trd>
  <trd lng="de">Pilz</trd>
  <trd lng="fr">champignon</trd>
</drv>
<drv mrk="fung.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Grupo de vegeta&jcirc;oj (<trd lng="la">Fungi</trd>)
      senklorofilaj, reprodukti&gcirc;antaj plej ofte per sporoj.
    </dif>
  </snc>
  <trd lng="de">Pilze</trd>
</drv>
<drv mrk="fung.lamen0o">
  <kap>lamen<tld/>o</kap>
  <snc>
    <dif>
      Fungo, kies ombrelforma fruktokorpo de
      sube prezentas multajn alcentrajn lamenojn.
    </dif>
  </snc>
  <ref tip="malprt" cel="agarik.0acoj">agarikacoj</ref>
  <trd lng="de">Bl&auml;tterpilz</trd>
  <trd lng="fr">agaric</trd>
</drv>
<drv mrk="fung.musx0o">
  <kap>mu&scirc;<tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="amanit.0o">amanito</ref>
      (<trd lng="la">Amanita muscaria</trd>).
    </dif>
  </snc>
<bld lok="../bld/musxfung.jpg">mu&scirc;<tld/>o</bld>
  <trd lng="de">Fliegenpilz</trd>
</drv>
<drv mrk="fung.tub0o">
  <kap>tub<tld/>o</kap>
  <snc>
    <dif>
      Fungo, kies ombrelforma fruktokorpo de
      sube prezentas multajn tubformajn truojn.
    </dif>
  </snc>
  <ref tip="malprt" cel="bolet.0acoj">boletacoj</ref>
  <trd lng="cs">h&rcaron;&iacute;bek</trd>
  <trd lng="de">R&ouml;hrling</trd>
</drv>
</art>
