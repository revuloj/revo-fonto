<art mrk="$Id: bovling.xml,v 1.9 2023/11/18 09:24:07 revo Exp $">
<kap>
  <rad>bovling</rad>/o
</kap>
<drv mrk="bovling.0o">
  <kap><tld/>o</kap>
  <snc mrk="bovling.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Usondevena speco de
      <ref tip="super" cel="kegl.0oludo.SPO">kegloludo</ref>
      ludata sur speciala areno per dek kegloj kaj pezaj globoj:
      <ekz>
        mi estas mallerta ludanto de <tld/>o
        <fnt>
        <aut>Vastalto</aut>,
        <vrk>
<url ref="http://www.vastalto.com/blog/">Akvosfero</url></vrk>,
        <lok>Bovlingo, 2005-12-20</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_o;&c_w;&c_l;&c_ib;&c_n;&c_g;</trd>
    <trd lng="bg">&c_b;&c_o;&c_u;&c_l;&c_i;&c_n;&c_g;</trd>
    <trd lng="de">Bowling</trd>
    <trd lng="en">bowling</trd>
    <trd lng="fa">&a_b;&a_w;&a_l;&f_y;&a_n;&f_g;</trd>
    <trd lng="fr">bowling</trd>
    <trd lng="it">bowling</trd>
    <trd lng="nl">bowling</trd>
    <trd lng="pl"><klr>gra w </klr>kr&eogonek;gle</trd>
    <trd lng="zh">&#x4FDD;&#x9F61;&#29699; <pr>b&#x1CE;ol&#xED;ngqi&#xFA;</pr></trd>
</snc>
</drv>
</art>

