<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mangrov.xml,v 1.5 2023/01/12 08:35:54 revo Exp $">
<kap>
  <rad>mangrov</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="mangrov.0">
  <kap><tld/>o</kap>
  <snc mrk="mangrov.0.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      Arbaro en mara flusejo, kie kreskas arboj
      kapablaj elporti salajn akvon kaj grundon,
      precipe rizoforoj:
      <ekz>
        la specoj de <tld/>oj ordinare distribui&gcirc;as nur en
        subtropikaj, a&ubreve; tropikaj landoj
        <fnt>
          <aut>&Scirc;. Hori</aut>
          <vrk><url ref="&STEB;biologio/la-volvotigo-de-amikeco.pdf">
          La Volvotigo de Amikeco</url></vrk>
          <lok>1965</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_g;&c_r;&c_a;&c_v;&c_y; &c_l;&c_je;&c_s;</trd>
  <trd lng="fr">mangrove</trd>
  <trdgrp lng="pl">
    <trd>namorzyny</trd>,
    <trd>mangrowia</trd>
  </trdgrp>
  <trd lng="ro">mangrove</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_a;&#x43D;&c_gu;&c_r;&c_i;</trd>,
    <trd>&#x43C;&c_a;&#x43D;&c_gu;&c_r;&#x43E;&c_v;&c_ib; &#x43B;&c_ib;&c_s;&c_i;</trd>
  </trdgrp>
</drv>
</art>

