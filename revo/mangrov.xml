<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mangrov.xml,v 1.7 2024/01/25 20:10:34 revo Exp $">
<kap>
  <rad>mangrov</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="mangrov.0">
  <kap><tld/>o</kap>
  <snc mrk="mangrov.0.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      <ref tip="super" cel="arb.0aro">Arbaro</ref> en mara flusejo, kie kreskas arboj
      kapablaj elporti salajn akvon kaj grundon,
      precipe rizoforoj:
      <ekz>
        en vila&gcirc;eto en Gvineo <klr>[&#x2026;]</klr> &scirc;i laboras pri konservado de la <tld/>a
        arbaro
        <fnt>
          <bib>Monato</bib>
          <aut>Den Drown</aut>
          <vrk><url ref="&Monato;/2003/008171.html">Elektoj</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        la specoj de <tld/>oj ordinare distribui&gcirc;as nur en
        subtropikaj, a&ubreve; tropikaj landoj
        <fnt>
          <aut>&Scirc;. Hori</aut>
          <vrk><url ref="&STEB;biologio/la-volvotigo-de-amikeco.pdf">
          La Volvotigo de Amikeco</url></vrk>
          <lok>1965</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_g;&c_r;&c_a;&c_v;&c_y; &c_l;&c_je;&c_s;</trd>
  <trd lng="de">Mangrove <klr>(Wald)</klr></trd>
  <trd lng="fr">mangrove</trd>
  <trdgrp lng="pl">
    <trd>namorzyny</trd>,
    <trd>mangrowia</trd>
  </trdgrp>
  <trd lng="ro">mangrove</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_a;&c_n;&c_gu;&c_r;&c_i;</trd>,
    <trd>&c_m;&c_a;&c_n;&c_gu;&c_r;&c_o;&c_v;&c_ib; &c_l;&c_ib;&c_s;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7D05;&#x6A39; <pr>h&#xF3;ngsh&#xF9;</pr></trd>,
    <trd>&#x7EA2;&#x6811; <pr>h&#xF3;ngsh&#xF9;</pr></trd>,
    <trd>&#x7D05;&#x6A39;&#x79D1; <pr>h&#xF3;ngsh&#xF9;k&#x113;</pr></trd>,
    <trd>&#x7EA2;&#x6811;&#x79D1; <pr>h&#xF3;ngsh&#xF9;k&#x113;</pr></trd>
  </trdgrp>
</drv>
</art>

