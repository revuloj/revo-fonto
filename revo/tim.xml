<drv mrk="tim.0ego">
  <kap><tld/>ego</kap>
  <snc>
    <dif>
      Forta <tld/>o:
      <ekz>
        Nepriskribebla <tld/>ego atakis min, sed &gcirc;i anka&ubreve; enver&scirc;is en
        min novan vivon, kiun mi jam delonge ne sentis en mi
        <fnt><bib>Fab3</bib>, <lok>Io</lok></fnt>;
      </ekz><ekz>
        &Scirc;in portis efektive kaj preska&ubreve; levis super la teron doloro
        kaj <tld/>ego <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="terur.0o.timego">teruro<sncref/></ref>
  <trdgrp lng="be">
    <trd>&c_m;&c_o;&c_c;&c_n;&c_y; <ind>&c_s;&c_t;&c_r;&c_a;&c_h;</ind></trd>,
    <trd>&c_zh;&c_a;&c_h;</trd>,
    <trd>&c_zh;&c_u;&c_d;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ja">&#24656;&#24598;  <pr>&#x304D;&#12423;&#12358;&#12405;</pr></trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j; <ind>&c_s;&c_t;&c_r;&c_a;&c_h;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="tim.0a">
  <kap><tld/>a</kap>
  <snc mrk="tim.0a.KOMUNE">
    <dif>
      <ref tip="dif" cel="tim.0ema"><tld lit="T"/>ema</ref>:
      <ekz>
        anta&ubreve; <tld/>a okulo potenci&gcirc;as e&ccirc; kulo
        <fnt><bib>PrV</bib></fnt>;
      </ekz><ekz>
        <tld/>a klientino
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga
          Butiko</url></vrk>
          <lok>En Kinematografejo</lok>
        </fnt>;
      </ekz><ekz>
        <tld/>a dubo
        <fnt><bib>Hamlet</bib></fnt>.
      </ekz>
