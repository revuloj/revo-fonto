<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>tim</rad>/i
    </kap>

    <drv mrk="tim.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
	<dif>
	  Senti malagrablan senton, ka&ubreve;zatan de efektiva a&ubreve; imaga minacanta
	  dan&gcirc;ero a&ubreve; malbono, kiun oni deziras eviti:
	  <ekz>
	    <tld/>i la doloron, la morton, la maron, la mokojn<fnt>Z</fnt>;
	  </ekz><ekz>
	    <tld/>i la patron, la &scirc;telistojn;
	  </ekz><ekz>
	    <tld/>u Dion<fnt>Z</fnt>;
	  </ekz><ekz>
	    korniko vundita propran voston <tld/>as
	    <fnt><bib>PrV</bib></fnt>;
	  </ekz><ekz>
	    mi <tld/>is konfesi tion;
	  </ekz><ekz>
	    li <tld/>is diri<fnt>Z</fnt>,
	  </ekz><ekz>
	    entrepreni voja&gcirc;on<fnt>Z</fnt>;
	  </ekz><ekz>
	    ne <tld/>u servi al ili<fnt>Z</fnt>;
	  </ekz><ekz>
	    mi <tld/>as, ke mi povas fari ian maljusta&jcirc;on al iu<fnt>Z</fnt>;
	  </ekz><ekz>
	    mi <tld/>as, ke oni vin a&ubreve;skultas<fnt>Z</fnt>;
	  </ekz><ekz>
	    la esperantistoj ne <tld/>is, ke la mondo kulpigos ilin pri la
	    deziro helpi al la unui&gcirc;o de la homaro<fnt>Z</fnt>.
	  </ekz>
	</dif>
      </snc>
      <snc>
	<dif>
	  Konsideri kiel balda&ubreve; okazontan ion malbonan kaj beda&ubreve;rindan;
	  anta&ubreve;timi:
	  <ekz>
	    <tld/>i ribelon;
	  </ekz><ekz>
	    <tld/>i pri sia vivo, hava&jcirc;o;
	  </ekz><ekz>
	    vane vi <tld/>as, ke mi volas enkonduki novan
	    ortografion<fnt>Z</fnt>;
	  </ekz><ekz>
	    mi <tld/>as, ke pluvos morga&ubreve;, ke li ne venos;
	  </ekz><ekz>
	    &ccirc;u ni devas <tld/>i, ke nia tuta laborado fari&gcirc;os vana<fnt>Z</fnt>?
	  </ekz><ekz>
	    mi <tld/>as, &ccirc;u tio ne estas son&gcirc;o;
	  </ekz><ekz>
	    mi <tld/>as, &ccirc;u post tiel belaj promesoj vi ne ka&scirc;as malpli
	    sukcesajn rezultatojn<fnt>B</fnt>;
	  </ekz><ekz>
	    mi <tld/>as, ke via indigno diktus al vi tro akrajn
	    vortojn<fnt>Z</fnt>;
	  </ekz><ekz>
	    ne ekzistas e&ccirc; la plej malgranda ka&ubreve;zo por <tld/>i, ke aperus ia
	    nova lingvo, kiu elpu&scirc;us Esperanton<fnt>Z</fnt>.
	  </ekz>
	</dif>
      </snc>
      <trd lng="cs">b&aacute;t se</trd>
      <trd lng="fr">f&uuml;rchten</trd>
      <trd lng="en">fear</trd>
      <trd lng="es">temer</trd>
      <trdgrp lng="nl">
	<trd>bang zijn voor</trd>,
	<trd>vrezen</trd>
      </trdgrp>
      <trd lng="ru">&c_b;&c_o;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </drv>

    <drv mrk="tim.0o">
      <kap><tld/>o</kap>
      <snc>
	<dif>
	  Sento de tiu, kiu <tld/>as:
	  <ekz>
	    pali&gcirc;i de <tld/>o;
	  </ekz><ekz>
	    <tld/>o al, anta&ubreve; iu, pro io;
	  </ekz><ekz>
	    <tld/>o al, anta&ubreve; la mizero;
	  </ekz><ekz>
	    la komenco de la sa&gcirc;o estas <tld/>o anta&ubreve; Dio<fnt>Z</fnt>;
	  </ekz><ekz>
	    la virtuloj estas sen<tld/>aj<fnt>Z</fnt>.
	  </ekz>
	</dif>
	<refgrp tip="sin">
	  <ref cel="terur.0o">teruro</ref>.
	</refgrp>
      </snc>
      <trdgrp lng="de">
	<trd>Furcht</trd>,
	<trd>Angst</trd>
      </trdgrp>
      <trdgrp lng="nl">
	<trd>vrees</trd>,
	<trd>angst</trd>
      </trdgrp>
      <trdgrp lng="ru">
	<trd>&c_s;&c_t;&c_r;&c_a;&c_h;</trd>,
	<trd>&c_b;&c_o;&c_ja;&c_z;&c_n;&c_mol;</trd>
      </trdgrp>
    </drv>

    <drv mrk="tim.0a0ema">
      <kap><tld/>a, <tld/>ema</kap>
      <snc>
	<dif>
	  Inklina al <tld/>o:
	  <ekz>
	    anta&ubreve; <tld/>a okulo potenci&gcirc;as e&ccirc; kulo
	    <fnt><bib>PrV</bib></fnt>;
	  </ekz><ekz>
	    <tld/>ulo <tld/>as e&ccirc; sian propran ombron<fnt>Z</fnt>;
	  </ekz><ekz>
	    <tld/>ema kiel kuniklo, leporo.
	  </ekz>
	</dif>
      </snc>
      <trdgrp lng="de">
	<trd>furchtsam</trd>,
	<trd>&auml;ngstlich</trd>
      </trdgrp>
      <trdgrp lng="nl">
	<trd>bang</trd>,
	<trd>vreesachtig</trd>
      </trdgrp>
      <trd lng="ru">&c_b;&c_o;&c_ja;&c_z;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
    </drv>

    <drv mrk="tim.mal0i">
      <kap>mal<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
	<dif>
	  Riski fari ion malgra&ubreve; ebla dan&gcirc;ero:
	  <ekz>
	    li mal<tld/>is nin insulti.
	  </ekz>
	</dif>
      </snc>
      <trd lng="nl">trotseren</trd>
      <trd lng="ru">&c_o;&c_s;&c_m;&c_je;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </drv>

    <drv mrk="tim.mal0a">
      <kap>mal<tld/>a</kap>
      <snc>
	<dif>
	  Malinklina al <tld/>o:
	  <ekz>
	    mal<tld/>aj kavaliroj sur spiregantaj &ccirc;evaloj
	    <fnt><bib>Fab4</bib>, <vrk>Plumo kaj inkujo</vrk></fnt>;
	  </ekz><ekz>
	    &Ccirc;u li ne estas tiu, kiun oni celas mortigi? Kaj jen li
	    parolas mal<tld/>e, kaj ili diras al li nenion
	    <fnt><bib>NT</bib>, <lok>&Joh; 7:25&ndash;26.</lok></fnt>.
	  </ekz>
	</dif>
	<refgrp tip="sin">
	  <ref cel="kuragx.0a">kura&gcirc;a</ref>,
	  <ref cel="risk">riskema</ref>,
	  <ref cel="auxdac.0a">a&ubreve;daca</ref>,
	  <ref cel="entrep">entreprenema</ref>,
	  <ref cel="arogan.0a">aroganta</ref>,
	  <ref cel="brav.0a">brava</ref>.
	</refgrp>
      </snc>
      <rim>
	Pri la sinonimoj vd
	<ref cel="kuragx.0a.RIM">Rimarkon &ccirc;e &leftquot;kura&gcirc;a&rightquot;</ref>.
      </rim>
      <trdgrp lng="nl">
	<trd>onbevreesd</trd>,
	<trd>stout<klr tip="ind"> (onbevreesd)</klr></trd>
      </trdgrp>
      <trdgrp lng="ru">
	<trd>&c_b;&c_je;&c_s;&c_s;&c_t;&c_r;&c_a;&c_sh;&c_n;&c_y;&c_j;</trd>,
	<trd>&c_s;&c_m;&c_je;&c_l;&c_y;&c_j;</trd>,
	<trd>&c_o;&c_t;&c_v;&c_a;&c_zh;&c_n;&c_y;&c_j;</trd>
      </trdgrp>
    </drv>

    <drv mrk="tim.0osento">
      <kap><tld/>osento</kap>
      <snc>
	<dif>
	  <ref tip="dif" cel="tim.0o">timo</ref>.
	</dif>
      </snc>
      <trd lng="nl">angstgevoel</trd>
      <trd lng="ru">&c_s;&c_t;&c_r;&c_a;&c_h;</trd>
    </drv>
  </art>
