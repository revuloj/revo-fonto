<vortaro>
  <art mrk="$Id: kovard.xml,v 1.14 2023/12/03 09:59:00 revo Exp $">
    <kap>
      <rad>kovard</rad>/a <fnt><bib>PIV1</bib></fnt>
    </kap>

    <drv mrk="kovard.0a">
      <kap><tld/>a</kap>
      <uzo tip="stl">RAR</uzo>
      <snc>
	<dif>
	  <ref tip="super" cel="nobl.mal0a">Malnobla</ref>,
	  kiu agas kruele a&ubreve; brutale kontra&ubreve; persono ne kapabla
	  sin defendi, a&ubreve; atakas el ka&scirc;ejo a&ubreve; protektejo, ne
	  elmetante sin mem al dan&gcirc;ero:
	  <ekz>
	    tiam fieco de la <tld/>a Feniksa ne evidenti&gcirc;is ankora&ubreve;
	    <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 69a, p. 471a</lok></fnt>;
	  </ekz><ekz>
	    <tld/>a murdo de &jcirc;urnalisto en Nord-Irlando
	    <fnt><aut>Garbhan MacAoidh</aut>: <vrk>&Jcirc;urnalisto murdita</vrk>,
	      <bib>Monato</bib>, <lok>jaro 2002a, numero 1a, p.
	      14a</lok></fnt>;
	  </ekz><ekz>
	    bati infanon estas <tld/>e <fnt><bib>PIV2</bib></fnt>.
	  </ekz>
	</dif>
	<refgrp tip="vid">
	  <ref cel="insid.0ema">insidema</ref>,
          <ref cel="kuragx.mal0a">malkura&gcirc;a</ref>
	</refgrp>
      </snc>
  <trd lng="be">&c_p;&c_o;&c_d;&c_l;&c_y;</trd>
  <trdgrp lng="ca">
        <trd>vil</trd>,
        <trd>innoble</trd>
      </trdgrp>
  <trdgrp lng="cs">
    <trd>podl&yacute;</trd>,
    <trd>zbab&ecaron;le krut&yacute;</trd>,
    <trd>zbab&ecaron;l&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>niedertr&auml;chtig</trd>,
    <trd>hinterh&auml;ltig</trd>,
    <trd>feige</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>l&acirc;che</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gy&aacute;va</trd>,
    <trd>hitv&aacute;ny</trd>,	  
    <trd>aljas</trd>,	  
  </trdgrp>
  <trd lng="nl">laf</trd>
  <trd lng="pl">nikczemny</trd>
  <trdgrp lng="pt">
    <trd>covarde</trd>,
    <trd>cobarde</trd>
  </trdgrp>
  <trd lng="ro">abject</trd>    
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_m;&c_a;&c_l;&c_o;&c_d;&c_u;&c_sh;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>podl&yacute;</trd>,
    <trd>zbabelo krut&yacute;</trd>,
    <trd>zbabel&yacute;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x61E6;&#x592B; <pr>nu&#xF2;f&#x16B;</pr></trd>,
    <trd>&#x80C6;&#x5C0F; <pr>d&#x1CE;nxi&#x1CE;o</pr></trd>,
    <trd>&#x5B6C; <pr>n&#x101;o</pr></trd>,
    <trd>&#x61E6;&#x5F31; <pr>nu&#xF2;ru&#xF2;</pr></trd>,
    <trd>&#x61E6;&#x602F; <pr>nu&#xF2;qi&#xE8;</pr></trd>
  </trdgrp>
</drv>
