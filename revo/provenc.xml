<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: provenc.xml,v 1.5 2022/06/15 20:33:28 revo Exp $">
    <kap>
      <rad>Provenc</rad>/o
        <fnt>
          <bib>FK</bib>
        </fnt>
    </kap>

<drv mrk="provenc.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="provenc.0o.regiono">
    <dif>
      <ref tip="lst" lst="voko:regionoj" cel="region.0o.land">Regiono</ref>
      en la sudoriento de <ref tip="malprt" cel="franc.F0ujo">Francujo</ref>,
      limigita de <ref cel="rodan.0o">Rodano</ref>, Alpoj kaj Mediteraneo:
      <ekz>
        <nom>Chassi&eacute;</nom>, unu el la plej gloraj juristoj de Francujo kaj poste prezidanto de la
        parlamento en la <tld/>o, fondis sian gloron per tio, ke li en la jaro 1522
        elpa&scirc;is kiel advokato de ratoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ref tip="prt" cel="marsejl.0o">Marsejlo</ref>, en <tld/>o, suda Francio,
        estas elektita kiel e&ubreve;ropa kultura &ccirc;efurbo por la jaro 2013
        <fnt>
          <bib>Monato</bib>
          <aut>Thierry Spanjaard kaj Esperanto-Marseille</aut>
          <vrk><url ref="&Monato;/2013/010999.php">Publika mono por privata koncerto</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        ba&ubreve;ksito estas ekspluatata en <tld/>o ekde 1894
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="&Monato;/2015/011584.php">Ru&gcirc;aj &scirc;limoj en Mediteraneo</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_P;&c_r;&c_a;&c_v;&c_a;&c_n;&c_s;</trd>
    <trd lng="de">Provence</trd>
    <trd lng="en">Provence</trd>
    <trd lng="fr">Provence</trd>
    <trd lng="pl">Prowansja</trd>
    <trd lng="ro">Proven&#539;a</trd>
  </snc>
  <trd lng="uk">&#x41F;&c_r;&#x43E;&c_v;&c_a;&#x43D;&c_s; (&c_ib;&c_s;&c_t;. &#x43E;&c_b;&#x43B;&c_a;&c_s;&c_t;&#x44C;)</trd>
</drv>

<drv mrk="provenc.0a">
  <kap>
    <tld lit="p"/>a
  </kap>
  <snc mrk="provenc.0a.rilata">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        la tradicia <tld lit="p"/>a rakonto, la fama &leftquot;Pastoralo&#x201D;
        <fnt>
          <bib>Monato</bib>
          <aut>Sylvain Lelarge</aut>
          <vrk><url ref="&Monato;/2004/008496.html">La argilaj najbaretoj de Jesuo provenca
          tradicio</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        la okcitana havas tri grandajn dialektojn, la sud-okcitanan, kun la langvedoka
        kaj la <tld lit="p"/>a, la gaskonan kaj la nord-okcitanan kun Limo&gcirc;io,
        A&ubreve;vernjo &gcirc;is Piemonto
        <fnt>
          <bib>Monato</bib>
          <aut>Clair Mah&eacute;</aut>
          <vrk><url ref="&Monato;/2006/008904.php">La okcitana lingvo</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_v;&c_a;&c_n;&c_s;&c_k;&c_ib;</trd>
  <trd lng="pl">prowansalski</trd>
  <trd lng="ro">provensal&abreve;</trd>
</drv>
  </art>

