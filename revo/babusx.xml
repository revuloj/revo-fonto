<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: babusx.xml,v 1.19 2021/03/04 19:18:48 revo Exp $">
<kap>
  <rad>babu&scirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="babusx.0o">
  <kap><tld/>o</kap>
  <snc mrk="babusx.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Speco de <ref tip="super" cel="pantof.0o">pantoflo</ref> sen
      posta&jcirc;o kaj sen kalkanumo:
      <ekz>
        rajdas Jusef en beduena burnuso, kun flavaj <tld/>oj
        <fnt><aut>J. Ribillard</aut>, <vrk>Vagado sub palmoj</vrk>, 
        <lok>UEA, 1956</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">babutxa</trd>
  <trdgrp lng="de">
    <trd>Babusche</trd>,
    <trd>Stoffpantoffel</trd>
  </trdgrp>
  <trd lng="en">slipper</trd>
  <trd lng="es">babucha</trd>
  <trdgrp lng="fa">
    <trd>&a_n;&a_ayn;&a_l;&f_y;&a_n;</trd>
    <trd>&f_k;&a_f;&a_sh; &a_d;&a_m;&f_p;&a_A;&f_y;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">babouche</trd>
  <trdgrp lng="he">
    <trd>&#x5E7;&#x5D1;&#x5E7;&#x5D1;</trd>,
    <trd>&#x5E1;&#x5E0;&#x5D3;&#x5DC;</trd>
  </trdgrp>
  <trd lng="hu">papucs<klr tip="ind"> (sarkatlan)</klr></trd>
  <trd lng="it">babbuccia</trd>
  <trdgrp lng="nl">
    <trd>muil<klr tip="ind"> (pantoffel)</klr></trd>,
    <trd>slof<klr tip="ind"> (pantoffel)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bambosz</trd>,
    <trd>papu&cacute;</trd>
  </trdgrp>
  <trd lng="pt">babucha</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_b;&c_u;&c_sh;&c_a;</trd>,
    <trd>&c_sh;&c_l;&c_jo;&c_p;&c_a;&c_n;&c_je;&c_c;</trd>
  </trdgrp>
</drv>
</art>
