<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kabuk</rad>/o
</kap>
<drv mrk="kabuk.0o">
  <kap><tld/>o</kap> <fnt><bib>EncJap</bib></fnt>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <dif>
      Japana tradicia <ref tip="super" cel="teatr.0o.belarto">teatro</ref>
      ludata de viraj roluloj kun konvenciaj &scirc;minka&jcirc;oj kaj
      pozoj, kun akompanaj &ccirc;anto kaj <ref tip="vid"
      cel="sxamisen.0o">&scirc;amisena</ref> muziko.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_b;&c_u;&c_k;&c_ib; <klr>(&c_ja;&c_p;&c_o;&c_n;&c_s;&c_k;&c_ib; &c_t;&c_e;&c_a;&c_t;&c_r;)</klr></trd>
  <trd lng="de">Kabuki-Theater</trd>
  <trdgrp lng="fr">
    <trd>kabuki</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_b;&c_u;&c_k;&c_i;</trd>
</drv>
</art>
