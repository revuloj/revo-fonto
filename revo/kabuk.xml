<kap>
   <rad>kabuk</rad>/o
   <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="kabuk.0o">
   <kap><tld/>o</kap> 
   <uzo tip="fak">TEA</uzo>
   <snc>
     <dif>
       Japana tradicia <ref tip="super" cel="teatr.0o.belarto">teatro</ref>
       ludata de viraj roluloj kun konvenciaj &scirc;minka&jcirc;oj kaj
       pozoj, kun akompanaj &ccirc;anto kaj <ref tip="vid" cel="sxamisen.0o">&scirc;amisena</ref> muziko:
      <ekz>
        japana kulturo <klr>[&#x2026;]</klr> sumo-luktado, <tld/>o kaj su&scirc;io
        <fnt>
          <bib>Monato</bib>
          <aut>Vus-Tit Zik</aut>
          <vrk><url ref="&Monato;/2009/009769.php">Iom da historio kaj iom da fiziko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Miyoshi Etsuo</nom> <klr>[&#x2026;]</klr> vicprezidas la Asocion 
        de Infana <tld lit="K"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=57799">Miyoshi Etsuo</url></lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_k;&c_a;&c_b;&c_u;&c_k;&c_ib; 
<klr>(&c_ja;&c_p;&c_o;&c_n;&c_s;&c_k;&c_ib; 
&c_t;&c_e;&c_a;&c_t;&c_r;)</klr></trd>
  <trd lng="ca">kabuki</trd>
  <trd lng="cs">kabuki</trd>
  <trd lng="de">Kabuki-Theater</trd>
  <trd lng="fi">kabuki(-teatteri)</trd>
  <trdgrp lng="fr">
     <trd>kabuki</trd>
   </trdgrp>
  <trd lng="he"><klr tip="amb">(&tav;&yod;&alef;&tet;&resh;&vav;&fnun;) 
</klr>&qof;&bet;&vav;&qof;&yod;</trd>
  <trd lng="hu">kabuki <klr tip="amb">(sz&iacute;nh&aacute;z)</klr></trd>
  <trd lng="ja">&#x6B4C;&#x821E;&#x4F0E;  <pr>&#x304B;&#x3076;&#x304D;</pr></trd>
  <trd lng="nl">kabuki</trd>
  <trd lng="pl">kabuki</trd>
  <trdgrp lng="pt">
     <trd>cab&uacute;qui</trd>,
     <trd>kabuki</trd>
   </trdgrp>
  <trd lng="ro">kabuki</trd> 
  <trd lng="ru">&c_k;&c_a;&c_b;&c_u;&c_k;&c_i;</trd>
  <trd lng="sk">kabuki</trd>
  <trd lng="uk">&#x43A;&#x430;&#x431;&#x443;&#x43A;&#x456;</trd>
  <trd lng="zh">&#x6B4C;&#x821E;&#x4F0E; <pr>g&#x113;w&#x1D4;j&#xEC;</pr></trd>
</drv>
