<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>roma</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="roma.0o">
  <kap><tld/>o</kap>
  <snc mrk="roma.0o.ETN">
    <dif>
      Ano de la gento plimulta inter <ref tip="malprt"
      cel="cigan.0o">ciganoj</ref>:
      <ekz>
        frisoj, soraboj, ciganoj (plejparte <tld/>oj)
        <fnt>
        <aut>S. Hartwig</aut>,
        <vrk>Re: Demando pri la Biblio</vrk>,
        <lok>soc.culture.esperanto, 2004-04-26</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="roma.0o.RIM_UZO">
    Iuj a&ubreve;toroj nomas &ccirc;iujn ciganojn indiferente <ctl>romaoj</ctl>,
    supozante ke la nomo <ctl>cigano</ctl> ial sonas mal&gcirc;entile.
    <aut>&jcirc;v</aut>
  </rim>
  <trdgrp lng="fr">
    <trd>rom<klr> (tzigane)</klr></trd>,
    <trd>romanichel</trd>
  </trdgrp>
  <trd lng="pt">cigano</trd>
</drv>
</art>

