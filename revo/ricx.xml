      <ekz>
        pereo por la malri&ccirc;uloj estas ilia mal<tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 10:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni sentas tian mankon kaj mal<tld/>on, se neniu ion diras
        &ccirc;etombe
        <fnt><bib>Kiso</bib>, <lok>tombeja arabesko</lok></fnt>;
      </ekz>
      <ekz>
        senkleraj homoj povas nur malbone kaj rande partopreni socian
        vivon:
        ili ne povas defendi siajn rajtojn kaj plej ofte restas en
        senelira mal<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Ipfelkofer Jomo</aut>
          <vrk><url ref="&Monato;/2003/008127.html">Per edukado
          por edukado</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="ca">pobresa</trd>
  <trd lng="de">Armut</trd>
  <trdgrp lng="en">
    <trd>poverty</trd>,
    <trd>penury</trd>
  </trdgrp>
  <trd lng="es">pobreza</trd>
  <trdgrp lng="fr">
    <trd>paup&eacute;risme</trd>,
    <trd>pauvret&eacute;</trd>
  </trdgrp>
  <trd lng="hu">szeg&eacute;nys&eacute;g</trd>
  <trd lng="id">ke<ind>miskin</ind>an</trd>
  <trd lng="nl">armoede</trd>
  <trd lng="pl">bieda</trd>
  <trd lng="ro">s&abreve;r&abreve;ciei</trd>
  <trd lng="ru">&c_b;&c_je;&c_d;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="tr">yoksulluk</trd>
</drv>

<drv mrk="ricx.mal0eco">
  <kap>mal<tld/>eco</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="ricx.mal0o">Mal<tld/>o</ref>:
      <ekz>
        lia sorto estis sperti malsaton kaj malvarmon, la premon kaj
        mizeron de mal<tld/>eco
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        bonklasaj homoj, kiuj falis en mal<tld/>econ
        <fnt><bib>FK</bib>, <lok>4, rakontoj, la hejmo de la metiisto,
        (de
        nov-jorka kuracisto, rerakontita de E. Weilsh&auml;user)</lok></fnt>;
      </ekz>
      <ekz>
        <ind>mal<tld/>eco ne estas malvirto</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="hu">nem b&udblac;n a szeg&eacute;nys&eacute;g</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="bo">&t_da;&t_ba;&t_shabchu;&t_la;&t_tsheg;&t_-a;&t_pha;&t_naro;&t_nga;&t_sa;&t_tsheg;</trd>
