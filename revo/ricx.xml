<drv mrk="ricx.mal0a">
  <kap><ofc>*</ofc>mal<tld/>a</kap>
  <snc mrk="ricx.mal0a.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Posedanta malmulte da valora hava&jcirc;o:
      <ekz>
        mal<tld/>a vila&gcirc;a virino
        <fnt><bib>F</bib>, <lok>&FE; 15, la feino (da&ubreve;rigo)</lok></fnt>;
      </ekz>
      <ekz>
        mi estas mizera kaj mal<tld/>a
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 40:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        plej mal<tld/>a almozulo
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        li havis belegajn longajn harojn, sed mal<tld/>ajn vestojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>a urbana domo
        <fnt>
          <bib>Fab3</bib>
          <lok>Juda knabino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>mal<tld/>a kiel muso pre&gcirc;eja</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="de"><ind>arm</ind> wie eine Kirchenmaus</trd>
