<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ri&ccirc;</rad>/a
</kap>
<drv mrk="ricx.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Posedanta multajn hava&jcirc;ojn, multan monon, multajn bienojn:
      <ekz>
        &scirc;ajnigi sin <tld/>a, nenion havante<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>eco estas frato de fiereco
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ne helpos <tld/>o en la tago de kolero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind>nov<tld/>ulo</ind>.
        <trd lng="ru">&c_n;&c_u;&c_v;&c_o;&c_r;&c_i;&c_sh;</trd>
      </ekz>
    </dif>
    <trd lng="ru">&c_b;&c_o;&c_g;&c_a;&c_t;&c_y;&c_j;</trd>
  </snc>
  <snc>
    <uzo tip="klr">(iu, io)</uzo>
    <dif>
      Havanta multe da io ajn:
      <ekz>
        li estas <tld/>a je scio kaj sperto;
      </ekz>
      <ekz>
        principo <tld/>a je konsekvencoj;
      </ekz>
      <ekz>
        <tld/>a je esperoj, je promesoj.
      </ekz>
    </dif>
    <trd lng="ru">&c_b;&c_o;&c_g;&c_a;&c_t;&c_y;&c_j;</trd>
  </snc>
  <snc>
    <uzo tip="klr">(io)</uzo>
    <dif>
      Enhavanta multajn konsistiga&jcirc;ojn:
      <ekz>
        <tld/>a preparo, sed povo avara
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>a havo<fnt>Z</fnt>,
      </ekz>
      <ekz>
        meblaro, materialo;
      </ekz>
      <ekz>
        <tld/>a sperto;
      </ekz>
      <ekz>
        tiuj semoj alportis <tld/>an rikolton<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a lingvo;
      </ekz>
      <ekz>
        nia lingvo havos la eblon ne sole <tld/>i&gcirc;adi, sed konstante
        pliboni&gcirc;adi<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="abund.0a">abunda</ref>,
      <ref cel="mank.mal0a">malmanka</ref>,
      <ref cel="flu.super0a">superflua</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="sxut.super0i">super&scirc;uti</ref>.
    </refgrp>
    <trdgrp lng="ru">
      <trd>&c_b;&c_o;&c_g;&c_a;&c_t;&c_y;&c_j;</trd>,
      <trd>&c_o;&c_b;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">bohat&yacute;</trd>
  <trd lng="de">reich</trd>
  <trd lng="en">rich</trd>
  <trd lng="nl">rijk</trd>
  <trd lng="sv">rik</trd>
</drv>

<drv mrk="ricx.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Posedanta malmulte da valora hava&jcirc;o:
      <ekz>
        mal<tld/>ulo parencojn ne havas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        mal<tld/>eco ne estas malvirto
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="mon.sen0a">senmona</ref>,
      <ref cel="hav">senhava</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="posx.0o">po&scirc;o</ref>,
      <ref cel="pregx.0ejo">pre&gcirc;ejo</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Posedanta malmulte da io ajn.
    </dif>
  </snc>
  <trd lng="de">arm</trd>
  <trd lng="en">poor</trd>
  <trd lng="nl">arm</trd>
  <trd lng="ru">&c_b;&c_je;&c_d;&c_n;&c_y;&c_j;</trd>
  <trd lng="sv">fattig</trd>
</drv>
</art>
