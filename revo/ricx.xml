<drv mrk="ricx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Stato de abundo da mono, da hava&jcirc;oj a&ubreve; da ajna enhavo:
      <ekz>
        feli&ccirc;o kaj <tld/>o envion elvokas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne helpos <tld/>o en la tago de kolero
        <fnt><bib>MT</bib>, <lok>&Sen; 11:4</lok></fnt>;
      </ekz>
      <ekz>
        altanima knabino proponas al li sian tutan <tld/>on da
        <klr>[&#x2026;]</klr> amo
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 17a, Adaha ser&ccirc;as
        Omaron</lok></fnt>.
      </ekz>
      <ekz>
        la iama &ccirc;efministro de Libano, Al-Hariri, <klr>[&#x2026;]</klr> fari&gcirc;is
        milionulo, sed liaj <tld/>oj ne restis en Libano kaj la lando
        apena&ubreve; profitis de liaj projektoj
        <fnt>
          <bib>Monato</bib>
          <aut>Husejn Al-Amily/pg</aut>
          <vrk><url ref="&Monato;/2006/008962.php">Reveno
          al la hejmlando</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        spirita <tld/>o ne akireblas en kursejo a&ubreve; sur foirego
        <fnt>
          <bib>Monato</bib>
          <aut>Christian Declerck</aut>
          <vrk><url ref="&Monato;/2009/009048.php">Spirituala
          materialismo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="posx.0o.monujo">po&scirc;o <sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_g;&c_a;&c_c;&c_mol;&c_c;&c_je;</trd>
  <trd lng="ca">riquesa</trd>
  <trdgrp lng="de">
    <trd lng="de">Reichtum</trd>
    <trd lng="de">reich sein</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>wealth</trd>,
    <trd>affluence</trd>
  </trdgrp>
  <trd lng="es">riqueza</trd>
  <trd lng="fr">richesse<klr tip="amb"> (situation)</klr></trd>
  <trd lng="hu">gazdags&aacute;g<klr tip="amb"> (&aacute;llapot)</klr></trd>
  <trd lng="id">ke<ind>kaya</ind>an</trd>
  <trd lng="nl">rijkdom</trd>
  <trdgrp lng="pl">
    <trd>bogactwo</trd>,
    <trd>dostatek</trd>,
    <trd>obfito&sacute;&cacute;</trd>
  </trdgrp>
  <trd lng="ro">bog&abreve;&#x21B;ie</trd>
  <trd lng="ru">&c_b;&c_o;&c_g;&c_a;&c_t;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>

<drv mrk="ricx.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Tre valora hava&jcirc;o:
      <ekz>
        da mono kaj <tld/>a&jcirc;o mi liveros al vi tiom, kiom vi povos
        porti
        <fnt><bib>ElektFab</bib>, <lok>Ursa Felo</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i posedas dek kvin milojn, grandan <tld/>a&jcirc;on
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        kun granda <tld/>a&jcirc;o vi reiras nun al viaj tendoj, kaj kun tre
        granda brutaro,
        kun ar&gcirc;ento kaj kun oro kaj kun kupro kaj kun fero, kaj kun tre
        multe da vestoj
        <fnt>
          <bib>MT</bib>
          <lok>Josuo 22:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        la arboj portos siajn fruktojn, la figarbo kaj vinberujo donos
        sian <tld/>a&jcirc;on
        <fnt>
          <bib>MT</bib>
          <lok>Joel 2:22</lok>
        </fnt>;
      </ekz>
      <ekz>
         ili nutras sin per la <tld/>a&jcirc;o de la maro
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 33:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj formoj, kiuj &scirc;ajnis al mi <tld/>a&jcirc;o, montri&gcirc;is nun en la
        praktiko
        senbezona balasto, tiel ekzemple mi devis for&jcirc;eti kelkajn
        nebezonajn sufiksojn
        <fnt>
          <bib>VivZam</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_g;&c_a;&c_c;&c_mol;&c_c;&c_je;</trd>
  <trd lng="de">Verm&ouml;gen</trd>
  <trd lng="en">wealth</trd>
  <trdgrp lng="fr">
    <trd>bien<klr tip="amb"> (richesse)</klr></trd>,
    <trd>richesse<klr tip="amb"> (bien)</klr></trd>
  </trdgrp>
  <trd lng="hu">gazdags&aacute;g<klr tip="amb"> (javak)</klr></trd>
  <trdgrp lng="id">
    <trd>harta</trd>,
    <trd>ke<ind>kaya</ind>an</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5BCC;  [<ind>&#x3068;&#x307F;</ind>]</trd>,
    <trd>&#x8CA1;&#x7523;  [<ind>&#x3056;&#x3044;&#x3055;&#x3093;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">vermogen</trd>
  <trd lng="pl">bogactwo<klr> (skarb)</klr></trd>
  <trd lng="ro">avere</trd>
  <trd lng="ru">&c_b;&c_o;&c_g;&c_a;&c_t;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="tr">zenginlik</trd>
  <trd lng="uk">&c_b;&c_a;&c_g;&c_a;&c_t;&c_s;&c_t;&c_v;&#x43E; (&#x43C;&c_a;&c_j;&#x43D;&#x43E;)</trd>
</drv>

<drv mrk="ricx.0eco">
  <kap><ofc>*</ofc><tld/>eco</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="ricx.0o"><tld lit="R"/>o</ref>:
      <ekz>
        la <tld/>eco de tiu &ccirc;i homo estas granda
        <fnt><bib>F</bib>, <lok>&FE; 35</lok></fnt>;
      </ekz>
      <ekz>
        la re&gcirc;o Salomono superis &ccirc;iujn re&gcirc;ojn de la tero per <tld/>eco
        kaj per sa&gcirc;eco
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 10:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        kontenteco estas pli bona ol <tld/>eco
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>eco estas frato de fiereco</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">a szerencse ritk&aacute;n j&aacute;r kev&eacute;lys&eacute;g n&eacute;lk&uuml;l</trd>
      </ekz>
      <ekz>
