<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tapi&scirc;</rad>/o
</kap>
<drv mrk="tapisx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tapisx.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Dika diversspeca teksa&jcirc;o destinita por kovri plankon a&ubreve; meblon:
      <ekz>
        <tld/>o kovris la tablon;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la ektagi&gcirc;o metas <tld/>on el perloj sur la herbejon.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_v;&c_a;&c_n;</trd>
  <trd lng="cs">koberec</trd>
  <trdgrp lng="en">
    <trd>carpet</trd>,
    <trd>rug</trd>
  </trdgrp>
  <trd lng="de">Teppich</trd>
  <trdgrp lng="fr">
    <trd>tapis</trd>,
    <trd>tapisserie</trd>,
    <trd>moquette</trd>
  </trdgrp>
  <trd lng="hu">sz&odblac;nyeg</trd>
  <trdgrp lng="nl">
    <trd>tapijt</trd>,
    <trd>vloerbedekking</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_v;&c_jo;&c_r;</trd>
  <trdgrp lng="sv">
    <trd>matta</trd>,
    <trd>bonad</trd>
  </trdgrp>
</drv>

<drv mrk="tapisx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="tapisx.0i.TEKS">
    <dif>
      Kovri per <tld/>o.
    </dif>
    <trd lng="en">carpet</trd>
  </snc>
  <snc mrk="tapisx.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kovri kvaza&ubreve; per <tld/>o:
      <ekz>
        la tero estis <tld/>ita de floroj.
      </ekz>
    </dif>
    <trd lng="en">blanket</trd>
  </snc>
  <trd lng="de"><klr>mit einem </klr>Teppich bedecken</trd>
  <trd lng="fr">tapisser</trd>
  <trd lng="hu">sz&odblac;nyeggel fed</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_k;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol; &c_k;&c_o;&c_v;&c_r;&c_o;&c_m;</trd>,
    <trd>&c_u;&c_s;&c_t;&c_i;&c_l;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
