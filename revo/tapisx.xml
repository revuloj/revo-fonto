<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tapi&scirc;</rad>/o
</kap>
<drv mrk="tapisx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Dika diversspeca teksa&jcirc;o destinita por kovri plankon a&ubreve; meblon:
      <ekz>
        <tld/>o kovris la tablon;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la ektagi&gcirc;o metas <tld/>on el perloj sur la herbejon.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">koberec</trd>
  <trd lng="de">Teppich</trd>
  <trdgrp lng="fr">
    <trd>tapis</trd>,
    <trd>tapisserie</trd>,
    <trd>moquette</trd>
  </trdgrp>
  <trd lng="hu">sz&odblac;nyeg</trd>
  <trdgrp lng="nl">
    <trd>tapijt</trd>,
    <trd>vloerbedekking</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_v;&c_jo;&c_r;</trd>
  <trdgrp lng="sv">
    <trd>matta</trd>,
    <trd>bonad</trd>
  </trdgrp>
</drv>

<drv mrk="tapisx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kovri per <tld/>o.
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kovri kvaza&ubreve; per <tld/>o:
      <ekz>
        la tero estis <tld/>ita de floroj.
      </ekz>
    </dif>
  </snc>
  <trd lng="de"><klr>mit einem </klr>Teppich bedecken</trd>
  <trd lng="fr">tapisser</trd>
  <trd lng="hu">sz&odblac;nyeggel fed</trd>
</drv>
</art>
