<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>krup</rad>/o
</kap>
<drv mrk="krup.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Laringa <ref tip="dif" cel="difter.0o">difterio</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_u;&c_p; <klr tip="amb">(&c_h;&c_v;&c_a;&c_r;&c_o;&c_b;&c_a;)</klr></trd>
  <trdgrp lng="de">
     <trd>Krupp</trd>,
     <trd>Kehlkopfdifterie</trd>,
     <trd>Kehlkopfdiphtherie</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>krupp</trd>,
     <trd>dift&eacute;ri&aacute;s <ind>torokgyullad&aacute;s</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_r;&c_u;&c_p; <klr>(&c_b;&c_o;&c_l;&c_je;&c_z;&c_n;&c_mol;)</klr></trd>
</drv>
</art>

