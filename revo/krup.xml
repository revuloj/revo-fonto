<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: krup.xml,v 1.20 2023/10/18 17:23:25 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>krup</rad>/o
</kap>
<drv mrk="krup.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="krup.0o.MED">
  <uzo tip="fak">MED</uzo>
    <dif>
      Laringa <ref tip="lst" cel="difter.0o" lst="voko:malsanoj">difterio</ref>:
      <ekz>
        kiam ni aliris al lia lito, mi a&ubreve;dis la senduban sonon de <tld/>o,
        kiu tute prave plenigis per tia timo la korojn de la gepatroj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        difterio <klr>[&#x2026;]</klr> plej ofte montri&gcirc;as kiel difteria laringi<klr>[t]</klr>o
        (<tld/>o), &ccirc;e kiu formi&gcirc;as sur la gor&gcirc;a mukozo falsaj membranoj,
        kiuj povas &scirc;topi la laringon kaj mortigi per asfiksio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=40782">Difterio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_u;&c_p; <klr tip="amb">(&c_h;&c_v;&c_a;&c_r;&c_o;&c_b;&c_a;)</klr></trd>
  <trdgrp lng="de">
    <trd>Krupp</trd>,
    <trd>Diphterie</trd>,
    <trd>obstruktive Laryngitis</trd>,
  </trdgrp>
  <trd lng="fr">croup</trd>
  <trdgrp lng="hu">
     <trd>krupp</trd>,
     <trd>dift&eacute;ri&aacute;s <ind>torokgyullad&aacute;s</ind></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30AF;&#x30EB;&#x30FC;&#x30D7;</trd>,
    <trd>&#x507D;&#x819C;&#x6027;&#x5589;&#x982D;&#x708E;  <pr>&#x304E;&#x307E;&#x304F;&#x305B;&#x3044;&#x306E;&#x3069;&#x3042;&#x305F;&#x307E;&#x3048;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kroep</trd>,
    <trd>strottenhoofddifterie</trd>
  </trdgrp>
  <trd lng="pl">d&lstroke;awiec <klr>(b&lstroke;onica krtani)</klr></trd>
  <trd lng="pt">crupe <klr tip="amb">(med.)</klr></trd>
  <trdgrp lng="ro">
    <trd>angin&abreve; difteric&abreve;</trd>,
    <trd>crup</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_r;&c_u;&c_p; <klr>(&c_b;&c_o;&c_l;&c_je;&c_z;&c_n;&c_mol;)</klr></trd>
  <trd lng="uk">&#x43A;&#x440;&#x443;&#x43F;</trd>
</drv>
</art>

