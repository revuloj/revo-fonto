        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=608956"
              >Dalek</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_ib;&c_b;&c_a;&c_r;&c_g;</trd>
    <trd lng="de">Cyborg</trd>
    <trd lng="en">cyborg</trd>
  </snc>
