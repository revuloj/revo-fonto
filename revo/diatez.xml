<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: diatez.xml,v 1.19 2023/10/14 11:26:53 revo Exp $">
<kap>
  <rad>diatez</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="diatez.0o">
  <kap><tld/>o</kap>
  <snc mrk="diatez.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="dif" cel="dispozici.0o.MED">Dispozicio<sncref/></ref>.
    </dif>
    <trd lng="be">&c_d;&c_y;&c_ja;&c_t;&c_e;&c_z;</trd>
    <trd lng="ca">di&agrave;tesi</trd>
    <trdgrp lng="cs">
      <trd>diat&eacute;ze</trd>,
      <trd>slovesn&yacute; rod</trd>
    </trdgrp>
    <trdgrp lng="de">
         <trd>Diathese</trd>,
         <trd>Neigung</trd>,
         <trd>Anlage</trd>
      </trdgrp>
    <trd lng="es">di&aacute;tesis</trd>
    <trdgrp lng="hu">
         <trd>hajlam</trd>,
         <trd>alkat<klr tip="ind"> (diat&eacute;zis)</klr></trd>,
         <trd>diat&eacute;zis</trd>
      </trdgrp>
    <trdgrp lng="pl">
        <trd>diateza</trd>,
        <trd>skaza<klr tip="amb"> (konstytucjonalna)</klr></trd>,
        <trd>konstytucja patologiczna</trd>,
        <trd>sk&lstroke;onno&sacute;&cacute;<klr> do
        zachorowa&nacute;</klr></trd>,
        <trd>podatno&sacute;&cacute;<klr> na zachorowania</klr></trd>
      </trdgrp>
    <trd lng="pt">di&aacute;tese</trd>
    <trd lng="ru">&c_d;&c_i;&c_a;&c_t;&c_je;&c_z;</trd>
    <trdgrp lng="sk">
      <trd>diat&eacute;za</trd>,
      <trd>n&aacute;chylnos&tcaron; k chorobe</trd>
    </trdgrp>
  </snc>
  <snc mrk="diatez.0o.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      <ref tip="dif" cel="vocx.0o.GRA">Vo&ccirc;o<sncref/></ref>:
      <ekz>
        la preciza elekto de la aplikinda <tld/>o
        &ccirc;u aktiva (Akt), pasiva (Pas) <klr>...</klr>
        <fnt>
          <aut>W. Jansen</aut>
          <vrk>Radikoj kaj vortoj en esperanto</vrk>
          <lok>Esperantologio / Esperanto Studies, 2013:6, p.
          9a-43a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_t;&c_a;&c_n; <klr tip="amb">(&c_l;&c_ib;&c_n;&c_g;&c_v;.)</klr></trd>
  </snc>
  <trd lng="fr">diath&egrave;se</trd>
  <trd lng="he">&#x5E0;&#x5D8;&#x5D9;&#x5D4; &#x5DC;&#x5DE;&#x5D7;&#x5DC;&#x5D5;&#x5EA;</trd>
  <trdgrp lng="ja">
    <trd>&#x7D20;&#x8CEA;  <pr>&#x305D;&#x3057;&#x3064;</pr></trd>,
    <trd>&#x4F53;&#x8CEA;  <pr>&#x305F;&#x3044;&#x3057;&#x3064;</pr></trd>,
    <trd>&#x7D20;&#x56E0;  <pr>&#x305D;&#x3044;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="uk">&c_d;&c_ib;&c_a;&c_t;&c_je;&c_z;</trd>
</drv>
</art>
