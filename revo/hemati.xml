<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hemati.xml,v 1.24 2023/11/15 16:45:18 revo Exp $">
<kap>
  <rad>hematit</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="hemati.0o">
  <kap><tld/>o</kap>
  <snc mrk="hemati.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Tre ofta <ref tip="super" cel="minera.0o">mineralo</ref>,
      ru&gcirc;a <ref tip="super" cel="oksid.0o">oksido</ref> de fero
      <frm>Fe<sub>2</sub>O<sub>3</sub></frm>,
      ekzistanta en multaj varioj kaj specoj, de belaj kristaloj
      &gcirc;is grajnetaj masoj:
      <ekz>
        la grundo de la monto <klr>[&#x2026;]</klr> konsistas el argilaj sabloj miksitaj kun
        feroksidoj: ru&gcirc;a <tld/>o kaj bruna limonito
        <fnt><aut>Robert Roze</aut>,
        <vrk><url ref="&Monato;/2000/006945.php">Flagrantaj koloroj</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/06, p. 17</lok>
        </fnt>;
      </ekz>      
      <ekz>
        la grundo dismovita faris lar&gcirc;ajn fran&gcirc;ojn de
        <tld/>a koloro
        <fnt>
          <aut>V. Catal&agrave;, trad. J. Ventura i Freixas</aut>,
          <vrk>Soleco</vrk>,
          <lok>1967</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_je;&c_m;&c_a;&c_t;&c_y;&c_t;</trd>,
    <trd>&c_ch;&c_y;&c_r;&c_v;&c_o;&c_n;&c_y; &c_zh;&c_a;&c_l;&c_ja;&c_z;&c_mol;&c_n;&c_ja;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hematit</trd>,
    <trd>krevel</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>H&auml;matit</trd>,
    <trd>Roteisenstein</trd>,
    <trd>Eisenglanz</trd>,
    <trd>Blutstein</trd>
  </trdgrp>
  <trd lng="el">&alfa;&jota;&my;&alfa;&tau;&jota_ton;&tau;&eta;&sigma_fina;</trd>
  <trd lng="en">hematite</trd>
  <trd lng="es">hematites</trd>
  <trd lng="fa">&a_h;&a_m;&a_A;&a_t;&f_y;&a_t;</trd>
  <trdgrp lng="fr">
    <trd>h&eacute;matite</trd>,
    <trd>oligiste</trd></trdgrp>
  <trdgrp lng="hu">
    <trd>v&ouml;r&ouml;svas&eacute;rc</trd>,
    <trd>hematit</trd>
  </trdgrp>
  <trd lng="it">ematite</trd>
  <trd lng="ja">&#x8D64;&#x9244;&#x9271;  <pr>&#x305B;&#x304D;&#x3066;&#x3063;&#x3053;&#x3046;</pr></trd>
  <trdgrp lng="nl">
    <trd>hematiet</trd>,
    <trd>ijzererts</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>hematyt</trd>,
    <trd>b&lstroke;yszcz &zdot;elaza</trd>,
    <trd>krwawnik<klr> (odmiana)</klr></trd>,
    <trd>&zdot;elaziak czerwony<klr> (odmiana)</klr></trd>
  </trdgrp>
  <trd lng="pt">hematita</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_je;&c_m;&c_a;&c_t;&c_i;&c_t;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_s;&c_n;&c_y;&c_j; &c_zh;&c_je;&c_l;&c_je;&c_z;&c_n;&c_ja;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hematit</trd>,
    <trd>krve&#x13E;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_je;&#x43C;&c_a;&c_t;&c_i;&c_t;</trd>,
    <trd>&c_ch;&c_je;&c_r;&c_v;&#x43E;&#x43D;&c_i;&c_j; &c_z;&c_a;&#x43B;&c_ib;&c_z;&#x43D;&#x44F;&#x43A;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8840;&#x5E08; <pr>xu&#xE8;sh&#x12B;</pr></trd>,
    <trd>&#x9808;&#x4E38; <pr>x&#x16B;w&#xE1;n</pr></trd>
  </trdgrp>
</drv>
</art>
