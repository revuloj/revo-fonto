<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hematit</rad>/o
</kap>
<drv mrk="hemati.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Tre ofta <ref tip="super" cel="minera.0o">mineralo</ref>, ru&gcirc;a
      oksido de fero, ekzistanta en multaj varioj
      kaj specoj, de belaj kristaloj &gcirc;is grajnetaj masoj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>H&auml;matit</trd>,
    <trd>Roteisenstein</trd>,
    <trd>Eisenglanz</trd>,
    <trd>Blutstein</trd>
  </trdgrp>
  <trd lng="en">hematite</trd>
  <trdgrp lng="nl">
    <trd>hematiet</trd>,
    <trd>ijzererts</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_je;&c_m;&c_a;&c_t;&c_i;&c_t;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_s;&c_n;&c_y;&c_j; &c_zh;&c_je;&c_l;&c_je;&c_z;&c_n;&c_ja;&c_k;</trd>
  </trdgrp>
</drv>
</art>
