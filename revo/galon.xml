<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>galon</rad>/o
</kap>
<drv mrk="galon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Dika mallar&gcirc;a rubando el oro, ar&gcirc;ento, silko a&ubreve; alia &scirc;tofo
      por ornami a&ubreve; borderi veston, &ccirc;apelon, kurtenon, uniformon. 
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Borde</trd>,
     <trd>Litze</trd>,
     <trd>Tresse</trd></trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_l;&c_u;&c_n;</trd>,
    <trd>&c_p;&c_o;&c_z;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_b;&c_o;&c_r;&c_d;&c_ju;&c_r;</trd>
  </trdgrp>
</drv>
<drv mrk="galon.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ornami, borderi per <tld/>o:
      <ekz><tld/>ita vesto;</ekz>
      <ekz>or<tld/>ita oficiro, lakeo.</ekz>
    </dif>
  </snc>
  <trd lng="de">betressen</trd>
</drv>
</art>
