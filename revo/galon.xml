<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: galon.xml,v 1.17 2015/07/04 08:37:34 revo Exp $">
<kap>
  <ofc>*</ofc> <rad>galon</rad>/o
</kap>
<drv mrk="galon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="galon.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="ruband.0o.ornamo">Rubando</ref> alkudrita al
      vesto, kurteno a&ubreve; alia por ornami a&ubreve; borderi &gcirc;in.
    </dif>
    <refgrp tip="vid">
      <ref cel="border.0ajxo">bordera&jcirc;o</ref>
      <ref cel="frangx.0o">fran&gcirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="ca">passam&agrave;</trd>
  <trdgrp lng="cs">
    <trd>galon</trd>,
    <trd>kaloun</trd>,
    <trd>lampas</trd>,
    <trd>prýmek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Borde</trd>,
    <trd>Litze</trd>,
    <trd>Tresse</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>galloon</trd>,
    <trd>cord</trd>,
    <trd>lace</trd>,
    <trd>piping</trd>,
    <trd>ribbon</trd>,
    <trd>stripe</trd>,
    <trd>tinsel</trd>,
    <trd>trim</trd>
  </trdgrp>
  <trd lng="fr">galon</trd>
  <trdgrp lng="hu">
    <trd>szeg&odblac;szalag</trd>,
    <trd>paszp&oacute;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>galon</trd>,
    <trd>bies</trd>
  </trdgrp>
  <trd lng="pl">galon<klr tip="ind"> (pasmanteria)</klr></trd>
  <trd lng="pt">gal&atilde;o<klr tip="ind"> (fita)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_l;&c_u;&c_n;</trd>,
    <trd>&c_p;&c_o;&c_z;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_b;&c_o;&c_r;&c_d;&c_ju;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>galôn</trd>,
    <trd>galún</trd>,
    <trd>ozdobná šnúra</trd>
  </trdgrp>
</drv>

<drv mrk="galon.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="galon.0i.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Ornami, borderi per <tld/>o:
      <ekz>
        <tld/>ita vesto;
      </ekz>
      <ekz>
        aperis <tld/>ita <klr>(kun <tld/>hava vesto)</klr> pordisto
        <fnt><bib>Fab4</bib>, <lok>historio el la dunoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">passamanar</trd>
  <trdgrp lng="cs">
    <trd>ozdobit prýmkem</trd>,
    <trd>premovat</trd>
  </trdgrp>
  <trd lng="de">betressen</trd>
  <trd lng="fr">galonner</trd>
  <trdgrp lng="hu">
    <trd>beszeg<klr tip="amb"> (szeg&odblac;szalaggal)</klr></trd>,
    <trd>paszp&oacute;loz</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>biezen</trd>,
    <trd>met <ind>galon</ind> beleggen</trd>
  </trdgrp>
  <trd lng="pl">galonem obszywa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>agaloar</trd>,
    <trd>galonar</trd>
  </trdgrp>
  <trd lng="sk">ozdobiť šnúrkou</trd>
</drv>
</art>
