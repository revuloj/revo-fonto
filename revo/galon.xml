<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: galon.xml,v 1.22 2023/10/15 06:07:00 revo Exp $">
<kap>
  <ofc>*</ofc> <rad>galon</rad>/o
</kap>
<drv mrk="galon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="galon.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="ruband.0o.ornamo">Rubando</ref> alkudrita al
      vesto, kurteno a&ubreve; alia por ornami a&ubreve; borderi &gcirc;in:
      <ekz>
        penu alporti iom da gloro en viaj <tld/>kovritaj vestoj
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        li ricevas sub la manon ian kanajlon kun oraj <tld/>oj, kiu falsas la le&gcirc;ojn
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvarangula, per la le&gcirc;aj ritpenikoj provizita tuko, kiu estis farita el blanka
        lano <klr>[&#x2026;]</klr>, ornamita per oraj <tld/>oj
        <fnt>
          <bib>RBa</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i surmetis al li ru&gcirc;an &ccirc;emizon kun galono sur la kolumo
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="border.0ajxo">bordera&jcirc;o</ref>,
      <ref cel="frangx.0o">fran&gcirc;o</ref>,
      <ref cel="pasame.0o">pasamento</ref>
    </refgrp>
  </snc>
  <ref tip="hom" cel="galjon.0o">galjono (mezurunuo)</ref>
  <trd lng="ca">passam&agrave;</trd>
  <trdgrp lng="cs">
    <trd>galon</trd>,
    <trd>kaloun</trd>,
    <trd>lampas</trd>,
    <trd>pr&yacute;mek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Borde</trd>,
    <trd>Litze</trd>,
    <trd>Tresse</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>galloon</trd>,
    <trd>cord</trd>,
    <trd>lace</trd>,
    <trd>piping</trd>,
    <trd>ribbon</trd>,
    <trd>stripe</trd>,
    <trd>tinsel</trd>,
    <trd>trim</trd>
  </trdgrp>
  <trd lng="fr">galon</trd>
  <trdgrp lng="hu">
    <trd>szeg&odblac;szalag</trd>,
    <trd>paszp&oacute;l</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x98FE;&#x308A;&#x3072;&#x3082;  <pr>&#x304B;&#x3056;&#x308A;&#x3072;&#x3082;</pr></trd>,
    <trd>&#x30E2;&#x30FC;&#x30EB;&#x7CF8;  <pr>&#x3082;&#x30FC;&#x308B;&#x3044;&#x3068;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>galon</trd>,
    <trd>bies</trd>
  </trdgrp>
  <trd lng="pl">galon<klr tip="ind"> (pasmanteria)</klr></trd>
  <trd lng="pt">gal&atilde;o<klr tip="ind"> (fita)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_l;&c_u;&c_n;</trd>,
    <trd>&c_p;&c_o;&c_z;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_b;&c_o;&c_r;&c_d;&c_ju;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>gal&ocirc;n</trd>,
    <trd>gal&uacute;n</trd>,
    <trd>ozdobn&aacute; &scaron;n&uacute;ra</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x491;&#x430;&#x43B;&#x443;&#x43D;</trd>,
    <trd>&#x43F;&#x43E;&#x437;&#x443;&#x43C;&#x435;&#x43D;&#x442;</trd>,
    <trd>&#x431;&#x43E;&#x440;&#x434;&#x44E;&#x440;</trd>
  </trdgrp>
</drv>

<drv mrk="galon.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="galon.0i.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Ornami, borderi per <tld/>o:
      <ekz>
        <tld/>ita vesto;
      </ekz>
      <ekz>
        aperis <tld/>ita <klr>(kun <tld/>hava vesto)</klr> pordisto
        <fnt><bib>Fab4</bib>, <lok>historio el la dunoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">passamanar</trd>
  <trdgrp lng="cs">
    <trd>ozdobit pr&yacute;mkem</trd>,
    <trd>premovat</trd>
  </trdgrp>
  <trd lng="de">betressen</trd>
  <trd lng="fr">galonner</trd>
  <trdgrp lng="hu">
    <trd>beszeg<klr tip="amb"> (szeg&odblac;szalaggal)</klr></trd>,
    <trd>paszp&oacute;loz</trd>
  </trdgrp>
  <trd lng="ja">&#x98FE;&#x308A;&#x3072;&#x3082;&#x3092;&#x3064;&#x3051;&#x308B;  <pr>&#x304B;&#x3056;&#x308A;&#x3072;&#x3082;&#x3092;&#x3064;&#x3051;&#x308B;</pr></trd>
  <trdgrp lng="nl">
    <trd>biezen</trd>,
    <trd>met <ind>galon</ind> beleggen</trd>
  </trdgrp>
  <trd lng="pl">galonem obszywa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>agaloar</trd>,
    <trd>galonar</trd>
  </trdgrp>
  <trd lng="sk">ozdobi&tcaron; &scaron;n&uacute;rkou</trd>
  <trd lng="uk">&#x43E;&#x431;&#x448;&#x438;&#x432;&#x430;&#x442;&#x438; &#x491;&#x430;&#x43B;&#x443;&#x43D;&#x43E;&#x43C;</trd>
</drv>
</art>
