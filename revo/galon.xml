<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc> <rad>galon</rad>/o</kap>
<drv mrk="galon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="ruband.0o.ornamo">Rubando</ref> alkudrita al
      vesto, kurteno a&ubreve; alia por ornami a&ubreve; borderi &gcirc;in.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Borde</trd>,
    <trd>Litze</trd>,
    <trd>Tresse</trd>
  </trdgrp>
  <trd lng="fr">galon</trd>
  <trdgrp lng="nl">
    <trd>galon</trd>,
    <trd>bies</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_l;&c_u;&c_n;</trd>,
    <trd>&c_p;&c_o;&c_z;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_b;&c_o;&c_r;&c_d;&c_ju;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="galon.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Ornami, borderi per <tld/>o:
      <ekz>
        <tld/>ita vesto;
      </ekz>
      <ekz>
        or<tld/>ita oficiro, lakeo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">betressen</trd>
  <trd lng="fr">galonner</trd>
  <trdgrp lng="nl">
    <trd>biezen</trd>,
    <trd>met <ind>galon</ind> beleggen</trd>
  </trdgrp>
</drv>
</art>
