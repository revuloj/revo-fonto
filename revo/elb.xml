<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Elb</rad>/o
</kap>
<drv mrk="elb.0o">
  <kap><tld/>o</kap>
  <snc mrk="elb.0o.rivero">
    <fnt><bib>PIV1</bib></fnt>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Nomo de rivero en <ref tip="vid" cel="german1.G0ujo">Germanujo</ref>.
    </dif>
    <trd lng="be">&c_E;&c_l;&c_mol;&c_b;&c_a; <klr>(&c_r;&c_e;&c_ch;&c_k;&c_a; &c_w; &c_N;&c_ja;&c_m;&c_je;&c_ch;&c_ch;&c_y;&c_n;&c_je;)</klr></trd>
    <trdgrp lng="de">
      <trd>Elbe</trd>
    </trdgrp>
    <trd lng="fr">Elbe<klr> (rivi&egrave;re)</klr></trd>
    <trd lng="hu">Elba</trd>
    <trd lng="nl">Elbe</trd>
    <trd lng="pt">Elba <klr tip="amb">(rio)</klr></trd>
    <trd lng="ru">&c_E;&c_l;&c_mol;&c_b;&c_a;</trd>
  </snc>
  <snc mrk="elb.0o.insulo">
    <fnt><bib>PIV1</bib></fnt>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Nomo de insulo en <ref tip="vid"
      cel="mediterane.0o">Mediteraneo</ref>, apartenanta al Italujo.
    </dif>
    <trd lng="be">&c_E;&c_l;&c_mol;&c_b;&c_a; <klr>(&c_m;&c_ib;&c_zh;&c_z;&c_je;&c_m;&c_n;&c_a;&c_m;&c_o;&c_r;&c_s;&c_k;&c_ib; &c_v;&c_o;&c_s;&c_t;&c_r;&c_a;&c_w;)</klr></trd>
    <trd lng="de">Elba</trd>
    <trd lng="fr">Elbe<klr> (ile)</klr></trd>
    <trd lng="hu">Elba-sziget</trd>
    <trd lng="nl">Elba</trd>
    <trd lng="pt">Elba <klr tip="amb">(ilha)</klr></trd>
    <trd lng="ru">&c_E;&c_l;&c_mol;&c_b;&c_a;</trd>
  </snc>
</drv>
</art>
