<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Elb</rad>/o
</kap>
<drv mrk="elb.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Nomo de rivero en
      <refgrp tip="vid">
        <ref cel="german1.G0ujo">Germanujo</ref>.
      </refgrp>
    </dif>
    <trdgrp lng="de">
      <trd>Elbe</trd>
    </trdgrp>
    <trd lng="nl">Elbe</trd>
    <trd lng="ru">&c_E;&c_l;&c_mol;&c_b;&c_a;</trd>
  </snc>
  <snc>
    <dif>
      Nomo de insulo en <ref tip="vid"
      cel="mediterane.0o">Mediteraneo</ref>, apartenanta al Italujo.
    </dif>
    <trd lng="de">Elba</trd>
    <trd lng="nl">Elba</trd>
    <trd lng="ru">&c_E;&c_l;&c_mol;&c_b;&c_a;</trd>
  </snc>
</drv>
</art>
