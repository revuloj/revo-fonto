<drv mrk="kart.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kart.0o.folio">
    <dif>
      Kartona a&ubreve; plasta folieto kun a&ubreve; por manaj a&ubreve;
      presaj surskriboj, magneta a&ubreve; elektronika registrado:
    </dif>
    <subsnc mrk="kart.0o.posxtkarto">
      <dif>
         <ref tip="dif" cel="kart.posxt0o.POSX">Po&scirc;t<tld/>o</ref>:
        <ekz>
          oni pakas la leterojn, po&scirc;tajn <tld/>ojn, po&scirc;tajn asignojn
          <fnt>
            <bib>FK</bib>
          </fnt>;
        </ekz>
        <ekz>
          la patrino de lia edzino transdonas al li la <tld/>ojn kaj
          tagolibrojn de sia mortinta edzo
          <fnt>
            <bib>FK</bib>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="de">Postkarte</trd>
    </subsnc>
    <subsnc mrk="kart.0o.dokumento">
      <dif>
        <ref tip="super" cel="dokume.0o">Dokumento</ref>, precipe
        tia havanta formon de <tld/>o<sncref ref="kart.0o.folio"/>,
        servanta kiel rekonilo, rajtigilo, identigilo:
        <ekz>
          jen estas mia <tld/>o, mi lo&gcirc;as sur la Suna Flanko
          <fnt>
            <bib>DL</bib>
          </fnt>;
        </ekz>
        <ekz>
          jen, prenu mian vizit<tld/>on &#8208; mi nun havas tutan aron da
          kompanioj
          <fnt>
            <bib>Monato</bib>
            <aut>Trevor Steele</aut>
            <vrk><url ref="&Monato;/2015/011474.php">Tridek jarojn
            poste</url></vrk>
            <lok>2015</lok>
          </fnt>;
        </ekz>
        <ekz>
          lia voja&gcirc;a <tld/>o estas notita por Saratovo
          <fnt>
            <bib>Revizoro</bib>
            <lok>Akto unua</lok>
          </fnt>;
        </ekz>
        <ekz>
          telefon<tld/>o
         <fnt><aut>Alica Komlo&scirc;iova</aut>,
        <vrk><url ref="&Monato;/2000/006927.php">Ne nur por infanoj</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/06, p. 21</lok>
        </fnt>;
        </ekz>
        <ekz>
          identiga <tld/>o
          <fnt>
            <bib>Monato</bib>
            <aut>Alice Liu</aut>
            <vrk><url ref="&Monato;/2014/011323.php">Bicikle al
            Nord-Koreio</url></vrk>
            <lok>2014</lok>
          </fnt>;
        </ekz>
        <ekz>
          havigi al la civitanoj novajn biometrikajn identec-<tld/>ojn
          kaj pasportojn
          <fnt>
            <bib>Monato</bib>
            <aut>Bardhyl Selimi</aut>
            <vrk><url ref="&Monato;/2009/009877.php">Vizoj
            reviziitaj</url></vrk>
            <lok>2009</lok>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="bilet.0o">bileto</ref>,
        <ref cel="paspor.0o">pasporto</ref>
      </refgrp>
      <trd lng="ca">carnet</trd>
      <trd lng="nl">identiteitskaart</trd>
      <trdgrp lng="pl">
        <trd>karta<klr> (przest.)</klr></trd>,
        <trd>papier<klr> (pot.)</klr></trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_u;&c_d;&c_o;&c_s;&c_t;&c_o;&c_v;&c_je;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>,
        <trd>&c_b;&c_i;&c_l;&c_je;&c_t;<klr tip="amb"> (&c_ch;&c_l;&c_je;&c_n;&c_s;&c_k;&c_i;&c_j; &c_i;
        &c_t;.&c_p;.)</klr></trd>,
        <trd>&c_t;&c_a;&c_l;&c_o;&c_n;</trd>
      </trdgrp>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="slip.0o">slipo</ref>
    </refgrp>
    <trd lng="ca">targeta</trd>
    <trd lng="es">tarjeta</trd>
