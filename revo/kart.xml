<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>kart</rad>/o
    </kap>
    <drv mrk="kart.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc>
        <dif>
          Kartonfolieto, uzata kiel korespondilo:
          <ekz>
            <ind>po&scirc;t<tld/>o</ind>;
            <trd lng="ru">&c_p;&c_o;&c_ch;&c_t;&c_o;&c_v;&c_a;&c_ja; &c_k;&c_a;&c_r;&c_t;&c_o;&c_ch;&c_k;&c_a;</trd>
          </ekz><ekz>
            ilustrita <tld/>o
            <klr>(kies unu flanko prezentas desegna&jcirc;on
              a&ubreve; fotografa&jcirc;on)</klr>.
          </ekz>  
        </dif>
        <trd lng="nl">kaart</trd>
      </snc>
      <snc>
        <dif>
          Kartonfolieto, oficiala a&ubreve; ne, kun surskriboj
          &ccirc;u presitaj, &ccirc;u manskribitaj, uzata kiel rekonilo:
          <ekz>
            <ind>nom<tld/>o</ind>, vizit<tld/>o
            <klr>(<tld/>o kun nomo kaj adreso)</klr>;
            <trd lng="en">business card</trd>
            <trd lng="nl">visitekaartje</trd>
            <trd lng="ru">&c_v;&c_i;&c_z;&c_i;&c_t;&c_n;&c_a;&c_ja; &c_k;&c_a;&c_r;&c_t;&c_o;&c_ch;&c_k;&c_a;</trd>
          </ekz><ekz>
            balotrajtiga <tld/>o;
          </ekz><ekz>
            legitimiga <tld/>o;
          </ekz><ekz>
            porcirkula <tld/>o
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="bilet.0o">bileto</ref>.
        </refgrp>
        <trd lng="nl">kaart</trd>
      </snc>
      <snc>
        <dif>
          <ref tip="dif" cel="mangx.0okarto">Man&gcirc;o<tld/>o</ref>:
          <ekz>
            man&gcirc;i la&ubreve; la <tld/>o
            <klr>(kiam la kliento elektas man&gcirc;a&jcirc;ojn sur
              la <tld/>o, kaj pagas sekve varian prezon)</klr>.
          </ekz> 
        </dif>
        <refgrp tip="sin">
          <ref cel="menu.0o.restoracia">menuo</ref>.
          <!--ref cel="fiks.0a">fiksa</ref-->
        </refgrp>
        <trd lng="nl">menu</trd>
        <trd lng="ru">&c_m;&c_je;&c_n;&c_ju;</trd>
      </snc>
      <snc>
        <dif>
          <ref tip="dif" cel="kart.lud0o">Lud<tld/>o</ref>:
          <ekz>
            meti ion sur la <tld/>on<fnt>Z</fnt>
            <klr>(riski)</klr>;
          </ekz><ekz>
            mi sentis, ke mi metas sur la <tld/>on la tutan
            estontan trankvilecon mian<fnt>Z</fnt>;
          </ekz><ekz>
            meti &ccirc;ion sur la lastan <tld/>on
            <klr>(riski la lastan &scirc;ancon)</klr>;
          </ekz><ekz>
            la vivo staras sur la <tld/>o<fnt>Z</fnt>
            <klr>(en ekstrema situacio)</klr>;
          </ekz><ekz>
            blindulo <tld/>ojn ludi ne devas<fnt>Z</fnt>
            <klr>(oni estas responda pri siaj eraroj)</klr>.
          </ekz>
        </dif>
        <trd lng="nl">kaart</trd>
      </snc>
      <snc>
        <dif>
          <ref tip="dif" cel="map.0o">Mapo</ref>.
        </dif>
        <trd lng="nl">landkaart</trd>
      </snc>
      <trd lng="cs">l&iacute;stek</trd>
      <trd lng="en">card</trd>
    </drv>
    <drv mrk="kart.0aro">
      <kap><tld/>aro</kap>
      <snc>
        <dif>
          Kompleto el 32 a&ubreve; 52 lud<tld/>oj.
        </dif>
      </snc>
      <trd lng="nl">spel <ind>kaarten</ind></trd>
      <trd lng="ru">&c_k;&c_o;&c_l;&c_o;&c_d;&c_a; <klr>(&c_k;&c_a;&c_r;&c_t;)</klr></trd>
    </drv>
    <drv mrk="kart.lud0o">
      <kap>lud<tld/>o</kap>
      <snc>
        <dif>
          Maldika kartonfolieto, rektangule tran&ccirc;ita,
          prezentanta sur unu flanko kolorajn gravura&jcirc;ojn,
          uzata por ludi diversajn ludojn (<tld/>ludojn):
          <ekz>
            niaj esperoj disfalis kiel dometo el lud<tld/>oj.
          </ekz>
        </dif>
      </snc>
      <trd lng="nl">speelkaart</trd>
      <trd lng="ru">&c_i;&c_g;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_k;&c_a;&c_r;&c_t;&c_a;</trd>
    </drv>
    <drv mrk="kart.0ludo">
      <kap><tld/>ludo</kap>
      <snc>
        <dif>
          Ludo per <ref tip="prt" cel="kart.lud0o">lud<tld/>oj</ref>:
          <ref tip="vid" cel="ermit.0ludo">ermitludo</ref>,
          <ref tip="sub" cel="brigx.0o">bri&gcirc;o</ref> kaj diversaj
          hazardludoj.
        </dif>
      </snc>
      <trd lng="nl">kaartspel</trd>
      <trd lng="ru">&c_k;&c_a;&c_r;&c_t;&c_o;&c_ch;&c_n;&c_a;&c_ja; &c_i;&c_g;&c_r;&c_a;</trd>
    </drv>
  </art>
