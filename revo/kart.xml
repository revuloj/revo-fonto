<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kart</rad>/o
</kap>
<drv mrk="kart.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kart.0o.folio">
    <dif>
      Kartona a&ubreve; plasta folieto kun manaj a&ubreve; presaj surskriboj, uzebla kiel
      rekonilo, identigilo, rajtigilo, korespondilo...
    </dif>
    <refgrp tip="vid">
      <ref cel="bilet.0o">bileto</ref>.
    </refgrp>
    <trd lng="nl">kaart</trd>
  </snc>
  <snc mrk="kart.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <ref tip="dif" cel="kart.lud0o">lud<tld/>o</ref>
    <ekz>
      meti ion sur la <tld/>on<fnt>Z</fnt>
      <klr>(riski)</klr>;
    </ekz>
    <ekz>
      mi sentis, ke mi metas sur la <tld/>on la tutan
      estontan trankvilecon mian<fnt>Z</fnt>;
    </ekz>
    <ekz>
      meti &ccirc;ion sur la lastan <tld/>on
      <klr>(riski la lastan &scirc;ancon)</klr>;
    </ekz>
    <ekz>
      la vivo staras sur la <tld/>o
      <klr>(en ekstrema situacio)</klr><fnt><bib>PrV</bib></fnt>;
    </ekz>
    <ekz>
      blindulo <tld/>ojn ludi ne devas
      <klr>(oni estas responda pri siaj
      eraroj)</klr><fnt><bib>PrV</bib></fnt>.
    </ekz>
    <trd lng="de">Spielkarte</trd>
    <trd lng="hu">j&aacute;t&eacute;kk&aacute;rtya</trd>
    <trdgrp lng="nl">
      <trd>speelkaart</trd>,
      <trd>kaart</trd>
    </trdgrp>
    <trd lng="ru">&c_i;&c_g;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_k;&c_a;&c_r;&c_t;&c_a;</trd>
  </snc>
  <snc mrk="kart.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <ref tip="dif" cel="mangx.0okarto">man&gcirc;o<tld/>o</ref>
    <ekz>
      man&gcirc;i la&ubreve; la <tld/>o
      <klr>(kiam la kliento elektas man&gcirc;a&jcirc;ojn sur
      la <tld/>o, kaj pagas sekve varian prezon)</klr>.
    </ekz>
    <refgrp tip="vid">
      <ref cel="menu.0o.restoracia">menuo</ref>.
      <!--ref cel="fiks.0a">fiksa</ref-->
    </refgrp>
    <adm>
      Koherigi kun "mangxokarto" kaj "menuo". Cxu ekzistas
      senca nuanco aux ne? La klarigo en ekz subkomprenigas,
      ke "mangxi laux la menuo" aux "laux la karto" estas
      malsamaj de la vidpunkto de prezo (kiel en la franca),
      sed tiu nuanco ne retrovigxas en la aliaj artikoloj.
      <aut>MB</aut>
    </adm>
    <trd lng="de">Speisekarte</trd>
    <trd lng="hu">&eacute;tlap</trd>
    <trd lng="nl">menu</trd>
    <trd lng="ru">&c_m;&c_je;&c_n;&c_ju;</trd>
  </snc>
  <snc mrk="kart.0o.GEOG_AST">
    <uzo tip="stl">ARK</uzo>
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">AST</uzo>
    <ref tip="dif" cel="map.0o">mapo</ref>
    <trd lng="de">Landkarte</trd>
    <trd lng="hu">t&eacute;rk&eacute;p</trd>
    <trdgrp lng="nl">
      <trd>landkaart</trd>,
      <trd>kaart</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">l&iacute;stek</trd>
  <trd lng="de">Karte</trd>
  <trd lng="en">card</trd>
  <trd lng="fr">carte</trd>
  <trd lng="hu">k&aacute;rtya</trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_t;&c_a;<klr tip="amb">
(&c_g;&c_je;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja;)</klr></trd>
</drv>

<drv mrk="kart.0aro">
  <kap><tld/>aro</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      Kompleto el 32 a&ubreve; 52 lud<tld/>oj.
    </dif>
  </snc>
  <trd lng="de">Kartenspiel</trd>
  <trd lng="nl">spel <ind>kaarten</ind></trd>
  <trd lng="fr">jeu de cartes<klr tip="ind"> (ensemble de
  cartes)</klr></trd>
  <trd lng="hu">k&aacute;rtya<klr tip="amb"> (egy csomag)</klr></trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_o;&c_d;&c_a; <klr>(&c_k;&c_a;&c_r;&c_t;)</klr></trd>
</drv>

<drv mrk="kart.balotrajtiga0o">
  <kap>balotrajtiga <tld/>o</kap>
  <uzo tip="stl">POL</uzo>
  <snc>
    <dif>
      Oficiala <tld/>o rajtiganta al vo&ccirc;dono en baloto.
    </dif>
  </snc>
  <trd lng="de">Wahlschein</trd>
  <trd lng="fr">carte d'&eacute;lecteur</trd>
  <trd lng="hu">v&aacute;laszt&oacute;i igazolv&aacute;ny</trd>
</drv>

<drv mrk="kart.bild0o">
  <kap>bild<tld/>o</kap>
  <uzo tip="fak">POSX</uzo>
  <snc>
    <dif>
      Karto ilustrita sur unu flanko, uzebla kiel <ref tip="vid"
      cel="kart.posxt0o">po&scirc;t<tld/>o</ref>, objekto de kolektismo, kaj
      ceteraj.
    </dif>
  </snc>
  <trd lng="de">Ansichtskarte</trd>
  <trd lng="fr">carte postale illustr&eacute;e</trd>
  <trd lng="hu">k&eacute;peslap</trd>
  <trdgrp lng="nl">
    <trd>postkaart</trd>,
    <trd>kaart</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_t;&c_k;&c_r;&c_y;&c_t;&c_k;&c_a;</trd>
</drv>

<drv mrk="kart.kredit0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      <tld lit="K"/>o por pagi el fiksita kredito a&ubreve; el bankokonto,
      ordinare uzebla nur kun sekreta numero.
    </dif>
  </snc>
  <trd lng="fr">carte de cr&eacute;dit</trd>
</drv>

<drv mrk="kart.lud0o">
  <kap>lud<tld/>o</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      <tld lit="K"/>o prezentanta sur unu flanko kolorajn gravura&jcirc;ojn de
      emblemoj kaj poentoj, uzata por ludi diversajn ludojn
      (<tld/>ludojn):
      <ekz>
        niaj esperoj disfalis kiel dometo el lud<tld/>oj.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kart.0o.SPO">karto <sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="de">Spielkarte</trd>
  <trd lng="fr">carte &agrave; jouer</trd>
  <trd lng="hu">j&aacute;t&eacute;kk&aacute;rtya</trd>
  <trdgrp lng="nl">
    <trd>speelkaart</trd>,
    <trd>kaart</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_g;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_k;&c_a;&c_r;&c_t;&c_a;</trd>
</drv>

<drv mrk="kart.nom0o">
  <kap>nom<tld/>o</kap>
  <snc>
    <ref tip="dif" cel="kart.vizit0o">vizit<tld/>o</ref>
  </snc>
  <trd lng="de">Visitenkarte</trd>
  <trd lng="en">business card</trd>
  <trd lng="fr">carte de visite</trd>
  <trd lng="hu">n&eacute;vjegyk&aacute;rtya</trd>
  <trd lng="nl">visitekaartje</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_i;&c_z;&c_i;&c_t;&c_n;&c_a;&c_ja; &c_k;&c_a;&c_r;&c_t;&c_o;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_i;&c_z;&c_i;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="kart.posxt0o">
  <kap>po&scirc;t<tld/>o</kap>
  <uzo tip="fak">POSX</uzo>
  <snc>
    <dif>
      Po&scirc;te sendata <tld/>o, kutime rekte surskribata kaj afrankata, sen
      koverto.
    </dif>
  </snc>
  <trd lng="de">Postkarte</trd>
  <trd lng="fr">carte postale</trd>
  <trd lng="hu">levelez&odblac;lap</trd>
  <trdgrp lng="nl">
    <trd>postkaart</trd>,
    <trd>kaart</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_ch;&c_t;&c_o;&c_v;&c_a;&c_ja; &c_k;&c_a;&c_r;&c_t;&c_o;&c_ch;&c_k;&c_a;</trd>
  <trd lng="vo">potakad</trd>
</drv>

<drv mrk="kart.telefon0o">
  <kap>telefon<tld/>o</kap>
  <snc>
    <dif>
      A&ccirc;etebla <tld/>o &scirc;ovebla en strattelefonon, provizanta eluzeblan
      da&ubreve;ron de komunikado:
      <ekz>
        oni povas telefoni anka&ubreve; en la telefonbudoj surtrotuare per
        telefon<tld/>oj de Kosovo
        <fnt><aut>Bardhyl Selimi</aut>: <vrk>Kosovo anta&ubreve;eniras</vrk>,
        <bib>Monato</bib>, <lok>jaro 2003a, numero 1a, p. 21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">carte t&eacute;l&eacute;phonique</trd>
</drv>

<drv mrk="kart.vizit0o">
  <kap>vizit<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="K"/>o kun nomo, adreso kaj aliaj personaj a&ubreve; profesiaj
      informoj, donata okaze de vizito a&ubreve; unua renkonto por faciligi
      postajn kontaktojn:
      <ekz>
        se mi scius la familinomon! Ne finlegis mi &gcirc;in sur la vizit<tld/>o
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 5a, p. 53a</lok></fnt>...
      </ekz>
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="kart.nom0o">nom<tld/>o</ref>
  </refgrp>
  <trd lng="de">Visitenkarte</trd>
  <trd lng="en">business card</trd>
  <trd lng="fr">carte de visite</trd>
  <trd lng="hu">n&eacute;vjegyk&aacute;rtya</trd>
  <trd lng="nl">visitekaartje</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_i;&c_z;&c_i;&c_t;&c_n;&c_a;&c_ja; &c_k;&c_a;&c_r;&c_t;&c_o;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_i;&c_z;&c_i;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="kart.0ludo">
  <kap><tld/>ludo</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      Ludo per <ref tip="prt" cel="kart.lud0o">lud<tld/>oj</ref>:
      <ref tip="vid" cel="ermit.0ludo">ermitludo</ref>,
      <ref tip="sub" cel="brigx.0o">bri&gcirc;o</ref> kaj diversaj
      hazardludoj.
    </dif>
  </snc>
  <trd lng="de">Kartenspiel</trd>
  <trd lng="hu">k&aacute;rtyaj&aacute;t&eacute;k</trd>
  <trd lng="nl">kaartspel</trd>
  <trd lng="fr">jeu de cartes<klr tip="ind"> (jeu)</klr></trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_t;&c_o;&c_ch;&c_n;&c_a;&c_ja; &c_i;&c_g;&c_r;&c_a;</trd>
</drv>
</art>
