<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>noc</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="noc.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <ref tip="dif" cel="util.mal0i">malutili</ref>
    <ekz>
      vi <klr>...</klr> devas mem faciligi la koron, por ne <tld/>i la
      farton
      <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 64a, p. 385-386a</lok></fnt>.
    </ekz>
    <refgrp tip="vid">
      <ref cel="difekt.0i">difekti</ref>,
      <ref cel="domagx.0i">doma&gcirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_k;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="es">
    <trd>da&ntilde;ar</trd>,
    <trd>perjudicar</trd>
  </trdgrp>
  <trd lng="fr">nuire</trd>
  <trdgrp lng="hu">
    <trd>&aacute;rt</trd>,
    <trd>k&aacute;ros</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_r;&c_je;&c_d;&c_i;&c_t;&c_mol;</trd>
</drv>
</art>

