<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: noc.xml,v 1.17 2024/01/29 15:59:31 revo Exp $">
<kap>
  <rad>noc</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="noc.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="noc.0i.malutili">
    <dif>
      <ref tip="dif" cel="util.mal0i">Malutili</ref>:
      <ekz>
        vi <klr>[&#x2026;]</klr> devas mem faciligi la koron, por ne <tld/>i la
        farton
        <fnt>
          <bib>RugxDom</bib> <lok>&ccirc;apitro 64a, p. 385-386a</lok>
        </fnt>;
      </ekz>
      <ekz>
        rompo de bara&jcirc;o retenanta toksan resta&jcirc;on de la minentrepreno <klr>[&#x2026;]</klr>
        <tld/>is 230 urbojn kaj 80 viva&jcirc;ajn speciojn
        <fnt>
        <bib>Monato</bib>
          <aut>Sidney Carlos Praxedes</aut>
          <vrk><url ref="&Monato;/2019/012427.php">Plej granda laborista
          akcidento</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la ofta ekspluatado de la kredo kaj de la Eklezioj anta&ubreve;vide al politikaj
        interesoj grave <tld/>is al la kredindeco de la kristana atesto
        <fnt>
          <vrk><url ref="http://www.ikue.org/ekumcarto.html">Ekumena &Ccirc;arto - Charta Oecumenica in
          Esperanto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="difekt.0i">difekti</ref>,
      <ref cel="damagx.0i">dama&gcirc;i</ref>,
      <ref cel="domagx.0i">doma&gcirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_k;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>danyar</trd>,
    <trd>perjudicar</trd>
  </trdgrp>
  <trd lng="cs">&scaron;kodit</trd>
  <trd lng="de">schaden</trd>
  <trdgrp lng="en">
    <trd>be injurious to</trd>,
    <trd>harm</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>da&ntilde;ar</trd>,
    <trd>perjudicar</trd>
  </trdgrp>
  <trd lng="fr">nuire</trd>
  <trdgrp lng="hu">
    <trd>&aacute;rt</trd>,
    <trd>k&aacute;ros</trd>
  </trdgrp>
  <trd lng="nl">schaden<klr tip="ind"> (zeldz.)</klr></trd>
  <trdgrp lng="pl">
    <trd>by&cacute; szkodliwym</trd>,
    <trd>szkodzi&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>prejudicar</trd>,
    <trd>ser <ind>prejudicial</ind> a</trd>,
    <trd>ser <ind>nocivo</ind> a</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>fii d&abreve;un&abreve;tor</trd>,
    <trd>avarie</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_r;&c_je;&c_d;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">&scaron;kodi&tcaron;</trd>
  <trd lng="tok">ike</trd>
  <trd lng="uk">&c_sh;&c_k;&c_o;&c_d;&c_i;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x635F;&#x5BB3; <pr>s&#x1D4;nh&#xE0;i</pr></trd>,
    <trd>&#x640D;&#x5BB3; <pr>s&#x1D4;nh&#xE0;i</pr></trd>,
    <trd>&#x5371;&#x5BB3; <pr>w&#x113;ih&#xE0;i</pr></trd>
  </trdgrp>
</drv>
</art>

