<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>1</ofc>
      <rad>fasad</rad>/o
    </kap>

    <drv mrk="fasad.0o">
      <kap><tld/>o</kap>

      <snc mrk="fasad.0o.ARKI">
        <uzo tip="fak">ARKI</uzo>
        <dif>
          Fronto de konstrua&jcirc;o, enhavanta la &ccirc;efan enirejon:
          <ekz>
            sur placo, ne malproksime de l' Kremlo, staras granda
            konstruo sur kies <tld/>o oni povas legi kvarlingve
            la faman frazon:
            <ctl>Proletoj el &ccirc;iuj landoj unui&gcirc;u</ctl>
            <fnt><bib>Lanti</bib>, <lok>Tri semajnoj en
            Rusio</lok></fnt>.
          </ekz>
        </dif>
        <trd lng="be">&c_f;&c_a;&c_s;&c_a;&c_d;</trd>
        <trd lng="bg">&c_f;&c_a;&c_s;&c_a;&c_d;&c_a;</trd>
        <trdgrp lng="de">
          <trd>Fassade</trd>,
          <trd>Frontseite</trd>,
          <trd>Stirnseite</trd>,
          <trd>Vorderseite</trd></trdgrp>
        <trd lng="en">facade</trd>
        <trd lng="es">fachada</trd>
        <trd lng="fr">fa&ccedil;ade</trd>
        <trd lng="hu">homlokzat</trd>
        <trd lng="it">facciata</trd>
        <trdgrp lng="nl">
          <trd>fa&ccedil;ade</trd>,
          <trd>voorgevel</trd>
        </trdgrp>
        <trdgrp lng="pl">
          <trd>fasada</trd>,
          <trd>fronton</trd>,
          <trd>front</trd>
        </trdgrp>
        <trdgrp lng="pt">
          <trd>fachada</trd>,
          <trd>frontaria</trd>,
          <trd>frontisp&iacute;cio <klr tip="amb">(de
          edif&iacute;cio)</klr></trd>,
          <trd>portada</trd>
        </trdgrp>
        <trd lng="ru">&c_f;&c_a;&c_s;&c_a;&c_d;</trd>
      </snc>

      <snc mrk="fasad.0o.KOMP">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Ekstera <ref tip="super" cel="fac.inter0o">interfaco</ref>
          de programo a&ubreve; komputa sistemo kun la
          ekstera mondo: kun la uzantoj, kun komputila reto ktp:
          <ekz>
            <tld/>o povas faciligi uzadon de
            ne<ref cel="afabl.0a.KOMP">afabla</ref> programo,
            diponigante menuojn ktp;
          </ekz><ekz>
            tiu fenestrosistemo disponigas Vindozan <tld/>on por
            Uniksaj programoj.
          </ekz>
        </dif>
        <trd lng="be">&c_ib;&c_n;&c_t;&c_e;&c_r;&c_f;&c_e;&c_j;&c_s;
        <klr
        tip="amb">(&c_p;&c_r;&c_a;&c_g;&c_r;&c_a;&c_m;&c_y;)</klr></trd>
        <trd lng="de">Benutzeroberfl&auml;che</trd>
        <trdgrp lng="en">
          <trd>front-end interface</trd>,
          <trd>look and feel</trd></trdgrp>
        <trd lng="hu">fel&uuml;let</trd>
        <trd lng="es">Interfaz de Usuario<klr> (computacional, de
        programas de computadora)</klr></trd>
        <trd lng="it">interfaccia</trd>
        <trd lng="pl">interfejs</trd>
        <trd lng="ru">&c_v;&c_n;&c_je;&c_sh;&c_n;&c_je;&c_je;
        &c_s;&c_o;&c_p;&c_r;&c_ja;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
      </snc>

      <adm>
        Supozeble ekzistas figura senco (trompa eksterajxo) kaj
        rilataj ekz-oj (lia gxentileco estas nura fasado, fasada 
        unuanimeco, ks), sed mi ne scias, cxu ili estas internaciaj
        kaj ne trovis ilin. <aut>MB</aut>
      </adm>
    </drv>

    <drv mrk="fasad.grafika0o">
      <kap>grafika <tld/>o</kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          <ref tip="super" cel="fasad.0o.KOMP">Fasado</ref>,
          homa-komputila interfaco en kiu por dialoga eneligo
          anstata&ubreve;
          vortoj estas uzataj bildoj.  Programo kun grafika <tld/>o
          ruli&gcirc;as sub fenestrosistemo kaj ofertas diversajn
          <ref tip="prt" cel="fenest.0ajxo">fenestra&jcirc;ojn</ref>:
          piktogramojn, butonojn, menuojn; la uzanto
          stiras &gcirc;in movante la muskursoron inter tiuj objektoj
          kaj
          alklakante bezonatajn opciojn.
        </dif>
        <trd lng="be">&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ch;&c_n;&c_y;
        <ind>&c_ib;&c_n;&c_t;&c_e;&c_r;&c_f;&c_e;&c_j;&c_s;</ind></trd>
        <trd lng="de">grafische Benutzeroberfl&auml;che</trd>
        <trdgrp lng="en">
          <trd>GUI</trd>,
          <trd>graphical user interface</trd>
        </trdgrp>
        <trd lng="hu">grafikus <ind>fel&uuml;let</ind></trd>
        <trd lng="es">Interfaz Gr&aacute;fica de Usuario</trd>
        <trdgrp lng="it">
          <trd>GUI</trd>,
          <trd><ind>interfaccia</ind> grafica</trd>
        </trdgrp>
        <trd lng="pl"><ind>interfejs</ind> graficzny</trd>
        <trdgrp lng="ru">
          <trd>&c_g;&c_r;&c_a;&c_f;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;
          &c_i;&c_n;&c_t;&c_je;&c_r;&c_f;&c_je;&c_j;&c_s;</trd>,
          <trd>&c_g;&c_r;&c_a;&c_f;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja;
          &c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a;</trd>
        </trdgrp>
      </snc>
    </drv>

  </art>
