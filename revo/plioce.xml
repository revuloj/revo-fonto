<art mrk="$Id$">
<kap>
  <rad>pliocen</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="plioce.0o">
  <kap><tld/>o</kap>
  <snc mrk="plioce.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
