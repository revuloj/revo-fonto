<art mrk="$Id: plioce.xml,v 1.23 2024/02/17 07:52:16 revo Exp $">
<kap>
  <rad>pliocen</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="plioce.0o">
  <kap><tld/>o</kap>
  <snc mrk="plioce.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Geologia <ref tip="lst" cel="epok.0o.periodo" lst="voko:geologiaj_epokoj" val="-5332000">epoko</ref> inter <ref cel="miocen.0o">mioceno</ref> kaj <ref cel="plejst.0o">plejstoceno</ref>,
      de 5,3 &gcirc;is 2,6 jarmilionoj anta&ubreve; nuntempo:
      <ekz>
        plii&gcirc;o de karbona dioksido <klr>...</klr> en
        <tld lit="P"/>o rezulti&gcirc;is en Tero pli varma je 2&deg;C
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://scienco.ir/2017/09/24/studi-malproksiman-estintecon-de-tero-povas-instrui-al-ni-lecionojn-pri-gia-estonteca-klimato/?i=1">
          Studi malproksiman estintecon de Tero...</url></vrk>,
          <lok>Scienco, 2017-09-24</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ib;&c_ja;&c_c;&c_e;&c_n;</trd>
  <trd lng="cs">plioc&eacute;n</trd>
  <trd lng="de">Plioz&auml;n</trd>
  <trd lng="en">Pliocene</trd>
  <trd lng="fa">&f_p;&a_l;&f_y;&a_w;&a_s;&a_n;</trd>
  <trd lng="fr">Plioc&egrave;ne</trd>
  <trd lng="hu">plioc&eacute;n</trd>
  <trdgrp lng="ja">
    <trd>&#x9BAE;&#x65B0;&#x4E16;  <pr>&#x305B;&#x3093;&#x3057;&#x3093;&#x305B;&#x3044;</pr></trd>,
    <trd>&#x9BAE;&#x65B0;&#x7D71; <pr>&#x305B;&#x3093;&#x3057;&#x3093;&#x3068;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">plioceen</trd>
  <trd lng="pl">pliocen</trd>
  <trd lng="pt">Plioceno</trd>
  <trd lng="ru">&c_p;&c_l;&c_i;&c_o;&c_c;&c_je;&c_n;</trd>
  <trd lng="sk">polic&eacute;n</trd>
  <trd lng="uk">&#x43F;&#x43B;&c_ib;&#x43E;&c_c;&c_je;&#x43D;</trd>
  <trd lng="zh">&#x4E0A;&#x65B0;&#x4E16; <pr>sh&#xE0;ngx&#x12B;nsh&#xEC;</pr></trd>
</drv>
