<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>krestomati</rad>/o
</kap>
<drv mrk="kresto.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Kolekto da prozaj kaj poeziaj pecoj, kompilita la&ubreve; pedagogia,
      instrua vidpunkto:
      <ekz>
        atentan tralegon de la Fundamenta <tld/>o mi rekomendas al
        &ccirc;iu<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="antolo.0o">antologio</ref>.
    </refgrp>
  </snc>
  <trd lng="en">chrestomathy</trd>
  <trd lng="nl">bloemlezing</trd>
  <trd lng="ru">&c_h;&c_r;&c_je;&c_s;&c_t;&c_o;&c_m;&c_a;&c_t;&c_i;&c_ja;</trd>
</drv>
</art>
