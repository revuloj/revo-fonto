<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>1</ofc>
<rad>eksport</rad>/i</kap>
<drv mrk="ekspor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Transporti en eksterlandon por vendado la komerca&jcirc;ojn a&ubreve; 
produkta&jcirc;ojn
      de iu lando:
      <ekz>
        Argentino e&ccirc; ne povas <tld/>i siajn mielon, pomojn kaj 
limonojn
        <fnt><aut>Ro Sartor</aut>: <vrk>
        Ploru pri Argentino!</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 2a, p. 8a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>exportieren</trd>,
      <trd>ausf&uuml;hren</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_k;&c_s;&c_p;&c_a;&c_r;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_v;&c_a;&c_z;&c_ib;&c_c;&c_mol; 
<klr>(&c_t;&c_a;&c_v;&c_a;&c_r;&c_y;, 
&c_k;&c_a;&c_p;&c_ib;&c_t;&c_a;&c_l;&c_y;)</klr></trd>
  </trdgrp>
  <trd lng="es">exportar</trd>
  <trd lng="fr">exporter</trd>
  <trdgrp lng="hu">
    <trd>export&aacute;l</trd>,
    <trd>kivisz</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>uitvoeren<klr tip="ind"> (exporteren)</klr></trd>,
    <trd>exporteren</trd>
  </trdgrp>
  <trd 
lng="ru">&c_e;&c_k;&c_s;&c_p;&c_o;&c_r;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="ekspor.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc mrk="ekspor.0o.eksportado">
    <ref tip="dif" cel="ekspor.0ado">eksportado</ref>
    <ekz>
      La <tld/>o estis favorata<fnt><aut>Bo Sandelin</aut>: <vrk>
      Anta&ubreve;merkantilisma ekonomia pensado</vrk>,
      <bib>Monato</bib><lok>jaro 2001a, numero 2a, p. 16a</lok></fnt>.
    </ekz>
    <trdgrp lng="be">
      <trd>&c_e;&c_k;&c_s;&c_p;&c_a;&c_r;&c_t;</trd>,
      <trd>&c_v;&c_y;&c_v;&c_a;&c_z; 
<klr>(&c_t;&c_a;&c_v;&c_a;&c_r;&c_a;&c_w;, 
&c_k;&c_a;&c_p;&c_ib;&c_t;&c_a;&c_l;&c_a;&c_w;)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>export&aacute;l&aacute;s</trd>,
      <trd>kivitel</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>export</trd>,
      <trd>uitvoer</trd>
    </trdgrp>
  </snc>
  <snc mrk="ekspor.0o.eksportajxo">
    <ref tip="dif" cel="ekspor.0ajxo">eksporta&jcirc;o</ref>
    <ekz>
      dum 1998 &ccirc;irka&ubreve; 20 % de la makedonaj <tld/>oj estis 
direktataj al
      Jugoslavio<fnt><aut>Evgeni Georgiev</aut>: <vrk>
      Esperiga komenco</vrk>,
      <bib>Monato</bib><lok>jaro 2000a, numero 4a, p. 11a</lok></fnt>...
    </ekz>
    <trd lng="be">&c_e;&c_k;&c_s;&c_p;&c_a;&c_r;&c_t;&c_n;&c_y; 
&c_t;&c_a;&c_v;&c_a;&c_r;</trd>
    <trdgrp lng="hu">
      <trd>export</trd>,
      <trd>kivitel</trd>
    </trdgrp>
    <trd lng="nl">exportgoed</trd>
  </snc>
  <trd lng="ru">&c_e;&c_k;&c_s;&c_p;&c_o;&c_r;&c_t;</trd>
</drv>

<drv mrk="ekspor.0ado">
  <kap><tld/>ado</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Transportado de lokaj produkta&jcirc;oj al eksterlando.
      <ekz>
        Negative efikis nur la interrompo de la danuba &scirc;ipveturado en 
la
        <tld/>ado de specifaj komerca&jcirc;oj
        <fnt><aut>Evgeni Georgiev</aut>: <vrk>
        Esperiga komenco</vrk>,
        <bib>Monato</bib><lok>jaro 2000a, numero 4a, p. 11a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_k;&c_s;&c_p;&c_a;&c_r;&c_t;</trd>,
    <trd>&c_v;&c_y;&c_v;&c_a;&c_z; 
<klr>(&c_t;&c_a;&c_v;&c_a;&c_r;&c_a;&c_w;, 
&c_k;&c_a;&c_p;&c_ib;&c_t;&c_a;&c_l;&c_a;&c_w;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Export</trd>,
    <trd>Ausfuhr</trd>
  </trdgrp>
  <trd lng="es">exportaci&oacute;n</trd>
  <trd lng="fr">exportation<klr> (acte)</klr></trd>
  <trdgrp lng="hu">
    <trd>export&aacute;l&aacute;s</trd>,
    <trd>kivitel</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>het <ind>exporteren</ind></trd>,
    <trd>het <ind>uitvoeren</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_k;&c_s;&c_p;&c_o;&c_r;&c_t;</trd>
</drv>

<drv mrk="ekspor.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Komerca&jcirc;o transportata al eksterlando por vendo.
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_s;&c_p;&c_a;&c_r;&c_t;&c_n;&c_y; 
&c_t;&c_a;&c_v;&c_a;&c_r;</trd>
  <trdgrp lng="de">
    <trd>Export</trd>,
    <trd>Exportgut</trd>,
    <trd>exportierte Ware</trd>,
    <trd>Ausfuhr</trd>,
    <trd>Ausfuhrgut</trd>,
    <trd>ausgef&uuml;hrte <ind>Ware</ind></trd>
  </trdgrp>
  <trd lng="fr">exportation<klr> (bien)</klr></trd>
  <trd lng="hu">export&aacute;ru</trd>
  <trd lng="nl">exportgoed</trd>
  <trd lng="ru">&c_e;&c_k;&c_s;&c_p;&c_o;&c_r;&c_t;&c_n;&c_y;&c_j; 
&c_t;&c_o;&c_v;&c_a;&c_r;</trd>
</drv>

<drv mrk="ekspor.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="komerc.0isto">Komercisto</ref>,
      kiu <tld/>adas:
      <ekz>
        <tld/>istoj plendas, ke ili spertas malfacila&jcirc;ojn, &ccirc;ar 
la kurzo
        de la novzelanda dolaro estas tro alta<fnt><aut>Donald
        Rogers</aut>: <vrk>Ekonomia miraklo a&ubreve;
        malsano?</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero 2a,
        p. 14a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Exporteur</trd>
  <trd lng="es">exportador</trd>
  <trdgrp lng="fr">
    <trd>exportateur</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>export&odblac;r</trd>,
    <trd>export&aacute;l&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>exporteur</trd>,
    <trd>uitvoerder</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_k;&c_s;&c_p;&c_o;&c_r;&c_t;&c_jo;&c_r;</trd>
</drv>
</art>
