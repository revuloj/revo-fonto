  </trdgrp>
  <trdgrp lng="de">
    <trd>Export</trd>,
    <trd>Ausfuhr</trd>
  </trdgrp>
  <trd lng="en">exportation</trd>
  <trd lng="es">exportaci&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>exportation<klr tip="ind"> (acte d'exporter)</klr></trd>,
    <trd>export</trd>
  </trdgrp>
  <trd lng="he">&#x5D9;&#x5B0;&#x5E6;&#x5D5;&#x5BC;&#x5D0;</trd>
  <trdgrp lng="hu">
    <trd>export&aacute;l&aacute;s</trd>,
    <trd>kivitel</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>het <ind>exporteren</ind></trd>,
    <trd>het <ind>uitvoeren</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>eksport</trd>,
    <trd>wyw&oacute;z</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_k;&c_s;&c_p;&c_o;&c_r;&c_t;</trd>
  <trd lng="sk">export</trd>
  <trdgrp lng="zh">
    <trd>&#x51FA;&#x53E3; <pr>ch&#x16B;k&#x1D2;u</pr></trd>,
    <trd>&#x8F38;&#x51FA; <pr>sh&#x16B;ch&#x16B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ekspor.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc mrk="ekspor.0ajxo.komercajxo">
    <dif>
      <ref tip="super" cel="komerc.0ajxo">Komerca&jcirc;o</ref> transportata al eksterlando por vendo:
      <ekz>vivi pli-malpli memstare kun nur minimumaj importa&jcirc;oj kaj
      <tld/>a&jcirc;oj
        <fnt>
          <aut>Brian Kaneen, Wally Du Temple</aut>
          <vrk><url ref="&LOdE;97-lode.htm#97-14">NOREK-22:
          plia tre agrabla semajnfino en Esperantujo</url></vrk>
          <bib>LOdE</bib>
          <lok>2002:11</lok>
        </fnt>;
      </ekz>
      <ekz>Usono ne plu akceptos niajn nederlandajn <tld/>a&jcirc;ojn
        <fnt>
          <aut>Gerrit Berveling</aut>
          <vrk>Holoka&ubreve;sto</vrk>
          <bib>Monato</bib>
          <lok>2001/05, p. 4</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_s;&c_p;&c_a;&c_r;&c_t;&c_n;&c_y;
  &c_t;&c_a;&c_v;&c_a;&c_r;</trd>
  <trd lng="ca">exportaci&oacute;<klr> (b&eacute;ns)</klr></trd>
