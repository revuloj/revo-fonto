    <trd>&#x8F38;&#x51FA;&#x5546;  <pr>&#x3086;&#x3057;&#x3085;&#x3064;&#x3057;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>exporteur</trd>,
    <trd>uitvoerder</trd>
  </trdgrp>
  <trd lng="pl">eksporter</trd>
  <trd lng="ru">&c_e;&c_k;&c_s;&c_p;&c_o;&c_r;&c_t;&c_jo;&c_r;</trd>
  <trd lng="sk">export&eacute;r</trd>
  <trd lng="uk">&c_je;&#x43A;&c_s;&#x43F;&#x43E;&c_r;&c_t;&c_je;&c_r;</trd>
  <trd lng="zh">&#x51FA;&#x53E3;&#x5546; <pr>ch&#x16B;k&#x1D2;ush&#x101;ng</pr></trd>
</drv>
