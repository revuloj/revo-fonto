<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>1</ofc>
      <rad>prefekt</rad>/o
    </kap>
    <drv mrk="prefek.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">POL</uzo>
      <snc mrk="prefek.0o.teritoria">
	<dif>
	  En kelkaj landoj (ekz-e Francujo), regna funkciulo,
	  administranta  pli malpli grandan teritorian unuon. 
	</dif>
	<refgrp tip="vid">
	  <ref cel="gubern1.0estro">guberniestro</ref>.
	</refgrp>
      </snc>
      <snc mrk="prefek.0o.HIS">
	<uzo tip="fak">HIS</uzo>
	<dif>
	  En <ref cel="romi.0o">Romio</ref>, titolo de altranga
	  oficiro a&ubreve; &scirc;tatoficisto el la kavalira a&ubreve; senata
	  stato.  
	</dif>
	<trd lng="lat">praefectus</trd>
      </snc>
      <trd lng="be">&c_p;&c_r;&c_e;&c_f;&c_e;&c_k;&c_t;</trd>
      <trd lng="de">Pr&auml;fekt</trd>
      <trd lng="fr">pr&eacute;fet</trd>
      <trd lng="hu">prefektus</trd>
      <trd lng="nl">prefect</trd>
  <trd lng="pt">prefeito <klr tip="amb">(de uma regi&atilde;o)</klr></trd>
      <trd lng="ru">&c_p;&c_r;&c_je;&c_f;&c_je;&c_k;&c_t;</trd>
    </drv>

    <drv mrk="prefek.0ejo">
      <kap><tld/>ejo<fnt>PIV</fnt></kap>
      <uzo tip="fak">POL</uzo>
      <snc>
	<dif>
	  Domo kie funkcias la <tld/>aj oficoj kaj servoj;
	  kaj metafore, tiuj oficoj kaj servoj mem:
          <ekz>
            kampuloj plendis en <tld/>ejo, kaj al la salamandroj estis
            severe malpermesite vagi sur la bordo ekster la tiel
            nomata salamandra zono
            <fnt><bib>Salamandroj</bib>, <lok></lok></fnt>.
          </ekz>
	</dif>
      </snc>
      <trd lng="be">&c_p;&c_r;&c_e;&c_f;&c_e;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>
      <trd lng="fr">pr&eacute;fecture</trd>
      <trd lng="hu">prefekt&uacute;ra</trd>
      <trdgrp lng="ru">
	<trd>&c_p;&c_r;&c_je;&c_f;&c_je;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>,
	<trd>&c_u;&c_p;&c_r;&c_a;&c_v;&c_a;</trd>
      </trdgrp>
    </drv>
	  
    <drv mrk="prefek.0ujo">
      <kap><tld/>ujo<fnt>PIV</fnt></kap>
      <uzo tip="fak">POL</uzo>
      <snc>
	<dif>
	  Regiono administrata de <tld/>o:
	  <ekz>
	    <tld/>ujo estas la plej granda administra subunuo en
	    Japanujo;
	  </ekz><ekz>
            en 111 a. K. estis starigita la prefektujo Dunhuang,
            okcidente de kiu estis Xinjiang kaj mezaziaj landoj
            <fnt>
              <aut>Pang Naying</aut>:
              <lok><url
 ref="http://www.china.org.cn/world/shi-sichou/sichou17.htm"
                  >Fascinanta Dunhuang</url></lok>
            </fnt>;
          </ekz><ekz>
            la programo inkluzivos vizitojn al la urbodomo por saluti
            la vicurbestron, <klr>...</klr>, al la vicestro de la
            <tld lit="P"/>ujo Kumamoto &ccirc;e la
            <tld lit="P"/>a Domo<klr>...</klr>
            <fnt>
              <lok><url
 ref="http://www.uea.org/esperanto_p/dokumentoj/komunikoj_aug2000/gaz98.htm"
                  >Gazetaraj Komunikoj de UEA, n-ro 98 (2000-10-09).</url></lok>
            </fnt>.
          </ekz>
	</dif>
      </snc>
      <trd lng="be">&c_p;&c_r;&c_e;&c_f;&c_e;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>
      <trd lng="en">prefecture</trd>
      <trdgrp lng="fr">
        <trd>d&eacute;partement<klr>(circonscript. administrative)</klr></trd>,
        <trd>r&eacute;gion<klr>(circonscript. administrative)</klr></trd>
      </trdgrp>
      <trd lng="hu">prefekt&uacute;ra</trd>
      <trd lng="ru">&c_p;&c_r;&c_je;&c_f;&c_je;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>
    </drv>
	  
  </art>
