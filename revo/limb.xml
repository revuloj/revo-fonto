<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>limb</rad>/o
</kap>
<drv mrk="limb.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="limb.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <uzo tip="fak">AST</uzo>
    <dif>
      Gradita rando de cirklo, &ccirc;e astronomia, geodezia a&ubreve;
      matematika instrumento.
    </dif>
    <trdgrp lng="de">
      <trd>Saum</trd>,
      <trd>Rand</trd>
    </trdgrp>
    <trd lng="en">limb</trd>
    <trd lng="fr">limbe</trd>
    <trdgrp lng="hu">
      <trd>fokbeoszt&aacute;s</trd>,
      <trd>limbusz</trd>
    </trdgrp>
    <trd lng="pt">limbo <klr>(t&eacute;cn.)</klr></trd> 
    <trd lng="ru">&c_l;&c_i;&c_m;&c_b;</trd>
  </snc>
  <snc mrk="limb.0o.de_folio">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Membraneca maldika parto de la folio senpere insertita sur la tigo
      a&ubreve; portita sur la petiolo:
      <ekz>
        folio estas karakterizata de la aran&gcirc;i&gcirc;o kaj dividi&gcirc;o de la ripoj en la <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17303">Ripo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        ligulo estas membreneca apendico, kiu &ccirc;e graminacoj kaj ciperacoj trovi&gcirc;as &ccirc;e la
        limo inter la <tld/>o kaj foliingo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=30495"
              >Ligulo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Blattspreite</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>limb</trd>,
      <trd>blade</trd>
    </trdgrp>
    <trd lng="fr">limbe</trd>
    <trd lng="pt">limbo <klr>(bot.)</klr></trd>
    <trd lng="ru">&c_v;&c_l;&c_a;&c_g;&c_a;&c_l;&c_i;&c_shch;&c_je; <klr tip="amb">(&c_l;&c_i;&c_s;&c_t;&c_a; &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>
  </snc>
  <snc mrk="limb.0o.de_floro">
  <uzo tip="fak">BOT</uzo>
    <dif>
      La disetendita parto de petalo a&ubreve; sepalo.
    </dif>
    <trdgrp lng="de">
      <trd>Kelchsaum</trd>,
      <trd>Limbus</trd>
    </trdgrp>
    <trd lng="en">limb</trd>
    <trd lng="fr">limbe</trd>
    <trd lng="nl">bladschijf</trd>
    <trd lng="pt">limbo</trd>
    <trd lng="ru">&c_r;&c_a;&c_s;&c_sh;&c_i;&c_r;&c_je;&c_n;&c_n;&c_a;&c_ja; &c_ch;&c_a;&c_s;&c_t;&c_mol; <klr tip="amb">(&c_l;&c_je;&c_p;&c_je;&c_s;&c_t;&c_k;&c_a;,
    &c_l;&c_i;&c_s;&c_t;&c_a;)</klr></trd>
  </snc>
</drv>
</art>
