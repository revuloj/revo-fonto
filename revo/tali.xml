<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tali.xml,v 1.23 2022/03/28 12:03:04 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tali</rad>/o
</kap>
<drv mrk="tali.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;irka&ubreve;o de la homa korpo &ccirc;e nivelo de koksoj.
    </dif>
    <trdgrp lng="de">
      <trd>Taille</trd>,
      <trd>G&uuml;rtelweite</trd>
    </trdgrp>
    <trd lng="en">waist</trd>
    <trd lng="es">cintura</trd>
    <trd lng="fr">taille</trd>
    <trd lng="hu">der&eacute;k</trd>
    <trd lng="id">pinggang</trd>
    <trdgrp lng="pl">
      <trd>talia</trd>,
      <trd>kibi&cacute;</trd>,
      <trd>stan</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>midja</trd>,
      <trd>liv</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Parto de la homa korpo ekde kolo &gcirc;is koksoj:
      <ekz>
        &scirc;i rektigis sian elastan <tld/>on;
      </ekz>
      <ekz>
        bel<tld/>a.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="brust.0o">brusto</ref>,
      <ref cel="tors.0o">torso</ref>,
      <ref cel="bust.0o">busto</ref>.
    </refgrp>
    <trd lng="hu">fels&odblac;test</trd>
    <trdgrp lng="pl">
      <trd>tu&lstroke;&oacute;w</trd>,
      <trd>korpus</trd>
    </trdgrp>
    <trd lng="ro">corp</trd>
  </snc>
  <ref tip="hom" cel="tali2.0o">talio</ref>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_l;&c_ib;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="bo">&#xF66;&#xF90;&#xF7A;&#xF51;&#xF0B;&#xF54;&#xF0B;</trd>
  <trdgrp lng="nl">
    <trd>taille</trd>,
    <trd>middel</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_l;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>driek</trd>,
    <trd>p&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pas <klr>(t&ecaron;la)</klr></trd>,
    <trd>&zcaron;ivot</trd>
  </trdgrp>
  <trd lng="uk">&#x442;&#x430;&#x43B;&#x456;&#x44F;</trd>
</drv>

<drv mrk="tali.0vesto">
  <kap><tld/>vesto</kap>
  <snc>
    <dif>
      Vesto malsupreniranta nur &gcirc;is la <tld/>o; brustvesto
    </dif>
    <refgrp tip="sin">
      <ref cel="kamizo.0o">kamizolo</ref>,
      <ref cel="vesxt.0o">ve&scirc;to</ref>,
      <ref cel="jak.0o">jako</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>halenka bl&uring;za</trd>,
    <trd>vesta do pasu</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>parte de cima</trd>
  </trdgrp>
  <trd lng="sk">vesta do p&aacute;su</trd>
  <trd lng="uk">&#x431;&#x43B;&#x443;&#x437;&#x430;</trd>
</drv>
</art>
