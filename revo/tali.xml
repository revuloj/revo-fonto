<art mrk="$Id: tali.xml,v 1.29 2024/07/19 16:11:21 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tali</rad>/o
</kap>
<drv mrk="tali.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;irka&ubreve;o de la homa korpo &ccirc;e nivelo de koksoj.
    </dif>
    <trdgrp lng="de">
      <trd>H&uuml;ftumfang</trd>,
      <trd>Taille</trd>,
      <trd>H&uuml;fte<klr> [H&uuml;ftma&szlig;]</klr></trd>,
      <trd>G&uuml;rtelweite</trd>
    </trdgrp>
    <trd lng="en">waist</trd>
    <trd lng="es">cintura</trd>
    <trd lng="ca">cintura</trd>
    <trd lng="fr">taille</trd>
    <trd lng="hu">der&eacute;k</trd>
    <trd lng="id">pinggang</trd>
    <trdgrp lng="pl">
      <trd>talia</trd>,
      <trd>kibi&cacute;</trd>,
      <trd>stan</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>midja</trd>,
      <trd>liv</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Parto de la homa korpo ekde kolo &gcirc;is koksoj:
      <ekz>
        &scirc;i rektigis sian elastan <tld/>on;
      </ekz>
      <ekz>
        bel<tld/>a.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="brust.0o">brusto</ref>,
      <ref cel="tors.0o">torso</ref>,
      <ref cel="bust.0o">busto</ref>.
    </refgrp>
    <trd lng="hu">fels&odblac;test</trd>
    <trdgrp lng="pl">
      <trd>tu&lstroke;&oacute;w</trd>,
      <trd>korpus</trd>
    </trdgrp>
    <trd lng="ro">corp</trd>
  </snc>
  <ref tip="hom" cel="tali2.0o">talio</ref>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_l;&c_ib;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="bo">&#3942;&#3984;&#3962;&#3921;&#3851;&#3924;&#3851;</trd>
  <trdgrp lng="ja">
    <trd>&#33012;  <pr>&#12393;&#12358;</pr></trd>,
    <trd>&#33012;&#20307;  <pr>&#12393;&#12358;&#12383;&#12356;</pr></trd>,
    <trd>&#33136;&#22238;&#12426;  <pr>&#12371;&#12375;&#12414;&#12431;&#12426;</pr></trd>,
    <trd>&#33136;  <pr>&#12371;&#12375;</pr></trd>,
    <trd>&#12454;&#12456;&#12473;&#12488;  <pr>&#12358;&#12360;&#12377;&#12392;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>taille</trd>,
    <trd>middel</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_l;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>driek</trd>,
    <trd>p&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pas <klr>(t&ecaron;la)</klr></trd>,
    <trd>&zcaron;ivot</trd>
  </trdgrp>
  <trd lng="uk">&c_t;&c_a;&c_l;&c_ib;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#33136;&#33007; <pr>y&#257;oku&agrave;</pr></trd>,
    <trd>&#33136;&#37096; <pr>y&#257;ob&ugrave;</pr></trd>,
    <trd>&#33136;&#36523; <pr>y&#257;osh&#275;n</pr></trd>,
    <trd>&#33136;&#32930; <pr>y&#257;ozh&#299;</pr></trd>,
    <trd>&#33136;&#38291; <pr>y&#257;oji&#257;n</pr></trd>,
    <trd>&#33136;&#38388; <pr>y&#257;oji&#257;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="tali.0vesto">
  <kap><tld/>vesto</kap>
  <snc>
    <dif>
      Vesto malsupreniranta nur &gcirc;is la <tld/>o; brustvesto
    </dif>
    <refgrp tip="sin">
      <ref cel="kamizo.0o">kamizolo</ref>,
      <ref cel="vesxt.0o">ve&scirc;to</ref>,
      <ref cel="jak.0o">jako</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_l;&c_ju;&c_z;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>halenka bl&uring;za</trd>,
    <trd>vesta do pasu</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#23142;&#20154;&#29992;&#33012;&#30528;  <pr>&#12405;&#12376;&#12435;&#12424;&#12358;&#12393;&#12358;&#12385;&#12419;&#12367;</pr></trd>,
    <trd>&#12508;&#12487;&#12451;&#12473;</trd>,
    <trd>&#12467;&#12523;&#12469;&#12540;&#12472;&#12517;</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>parte de cima</trd>
  </trdgrp>
  <trd lng="sk">vesta do p&aacute;su</trd>
  <trd lng="uk">&c_b;&c_l;&c_u;&c_z;&c_a;</trd>
</drv>
</art>
