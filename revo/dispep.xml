<art mrk="$Id: dispep.xml,v 1.23 2022/03/15 16:19:58 revo Exp $">
  <kap>
    <ofc>1</ofc>
    <rad>dispepsi</rad>/o
  </kap>
<drv mrk="dispep.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Malfacila k doloriga <ref tip="super" cel="digest.0o">digesto</ref>:
      <ekz>
        la konsumado en restoracivagono al <tld/>uloj ka&ubreve;zas plian elspezon pro kuracado de trajna kuracisto
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="&Monato;/2005/008743.php">Konsumado sur trajnoj multekostas</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        surprizas la nombro de trafikaj <tld/>oj en Britio
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="&Monato;/2011/010431.php">Tamtamas &scirc;topi&gcirc;oj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>
  </snc>
  <trd lng="bg">&c_d;&c_i;&c_s;&c_p;&c_je;&c_p;&c_s;&c_i;&c_ja;</trd>
  <trd lng="ca">disp&egrave;psia</trd>
  <trd lng="cs">dyspepsie</trd>
  <trdgrp lng="de">
     <trd>Verdauungsst&ouml;rung</trd>,
     <trd>Dyspepsie<klr> [fak]</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>dyspepsia</trd>,
    <trd>indigestion</trd>
  </trdgrp>
  <trd lng="es">dispepsia</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_w;&a_hamza;&a_h;&a_A;&a_D;&a_m;&a_h;</trd>,
    <trd>&a_b;&a_d;&f_g;&a_w;&a_A;&a_r;&f_y;</trd>,
    <trd>&a_A;&a_kh;&a_t;&a_l;&a_A;&a_l; &a_h;&a_D;&a_m;</trd>,
  </trdgrp>
  <trd lng="fr">dyspepsie</trd>
  <trdgrp lng="hu">
    <trd>diszpepszia</trd>,
    <trd>em&eacute;szt&eacute;si zavar</trd>
  </trdgrp>
  <trd lng="id">dispepsia</trd>
  <trdgrp lng="nl">
    <trd>indigestie</trd>,
    <trd>slechte <ind>spijsvertering</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>niestrawno&sacute;&cacute;</trd>,
    <trd>dyspepsja</trd>
  </trdgrp>
  <trd lng="pt">dispepsia</trd>
  <trd lng="ro">indigestie</trd> 
  <trdgrp lng="ru">
    <trd>&c_d;&c_i;&c_s;&c_p;&c_je;&c_p;&c_s;&c_i;&c_ja;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_v;&c_a;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">porucha tr&aacute;venia</trd>
  <trd lng="uk">&#x434;&#x438;&#x441;&#x43F;&#x435;&#x43F;&#x441;&#x456;&#x44F;</trd>
</drv>
