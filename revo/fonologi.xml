<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>fonologi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="fonologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="fonologi.0o.SCI">
    <uzo tip="fak">FON</uzo>
    <uzo tip="fak">LIN</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Bran&ccirc;o de <ref tip="malprt" 
      cel="lingv.0oscienco.LIN">lingvoscienco</ref> kiu studas la 
      <ref tip="vid" cel="sistem.son0o.FON">sonsistemojn</ref> de 
      lingvoj:
      <ekz>
        <tld/>o fakte ne temas pri sonoj, sed pri la gramatika sistemo 
        de la sonoj
        <fnt>
          <aut>J. E. Abrahamsen</aut>,
          <vrk><url ref="http://www.ikso.net/ric/faktoj/esp_facil.php">&Ccirc;u Esperanto estas facila?</url></vrk>,
          <lok>[vidita je 2006-06-21]</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="foneti.0o.LIN">fonetiko <sncref/></ref>
  </refgrp>
  <trd lng="en">phonology</trd>
  <trd lng="es">fonolog&iacute;a</trd>
  <trd lng="fr">phonologie</trd>
  <trd lng="it">fonologia</trd>
</drv>

<drv mrk="fonologi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="fonologi.0isto.SCI">
    <dif>
      Fakulo pri <tld/>o:
      <ekz>
        kutime <tld/>istoj <klr>...</klr> disigas la silabon en du 
        partoj: la atako kaj la rimo
        <fnt>
          <aut>M. van Oostendorp</aut>,
          <vrk><url ref="http://ourworld.compuserve.com/homepages/profcon/m_medsi.htm">Mediteraneaj silaboj kaj la universala fonologio</url></vrk>,
          <lok>[prelego &ccirc;e UK 1998]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">phonologiste</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>