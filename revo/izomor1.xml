<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
   <art mrk="$Id$">
     <kap>
       <rad>izomorfi</rad>/o
     </kap>
     <drv mrk="izomor.0o">
       <kap><tld/>o<fnt>KompLeks</fnt></kap>
       <snc>
         <uzo tip="fak">MAT</uzo>
         <dif>
           <ref cel="bijekc.0a">Bijekcia</ref>
           <ref tip="super" cel="homomo.0o">homomorfio</ref> :
           <ekz>
             anka&ubreve; la <ref cel="bild.inversa0igo">inverso</ref> de
             <tld/>o estas homomorfio.
           </ekz>
         </dif>
       </snc>
       <trd lng="fr">isomorphisme</trd>
       <trd lng="ru">&c_i;&c_z;&c_o;&c_m;&c_o;&c_r;&c_f;&c_i;&c_z;&c_m;</trd>
     </drv>

     <drv mrk="izomor.0a">
       <kap><tld/>a</kap>
       <snc>
         <uzo tip="fak">MAT</uzo>
         <dif>
           <klr>(similaj sencoj kiel &ccirc;e
           <ref cel="homomo.0a">homomorfia</ref>)</klr>.
         </dif>
       </snc>
       <trd lng="fr">isomorphe</trd>
       <trd lng="ru">&c_i;&c_z;&c_o;&c_m;&c_o;&c_r;&c_f;&c_n;&c_y;&c_j;</trd>
     </drv>

   </art>

<!--
$Log$
-->
</vortaro>