<kap>
  <rad>kromofotografi</rad>/o
</kap>
<drv mrk="kromof.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">FOT</uzo>
    <dif>
      <ref tip="super" cel="fotogr.0arto">Fotografarto</ref>, evoluigata ek de la dua duono de la
      19a jc,
      en kiu oni kolorigis a&ubreve; produktis kolorajn bildojn:
      <ekz>
        terpomamelo estis utiligata &ccirc;e unu el la unuaj teknikoj de kolora fotografio,
        <tld/>o patentigita fare de la fratoj <nom>Lumi&egrave;re</nom>
        <fnt>
          <aut>Filomena Canzano</aut>
          <vrk><url ref="https://tevainfo.files.wordpress.com/2013/10/ev2012_1_rete.pdf">Terpomoj  &dash;
          Kurioza&jcirc;oj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_r;&c_o;&c_m;&c_a;&c_f;&c_a;&c_t;&c_a;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>
  <trd lng="ca">cromofotografia</trd>
  <trdgrp lng="de">
     <trd>Chromophotographie</trd>,
     <trd>Chromofotografie</trd>
  </trdgrp>
  <trd lng="en">color <ind>photography</ind></trd>
  <trd lng="fr">chromophotographie</trd>
  <trd lng="hu">sz&iacute;nes <ind>f&eacute;nyk&eacute;pez&eacute;s</ind></trd>
  <trd lng="ja">&#x5929;&#x7136;&#x8272;&#x5199;&#x771F;&#x8853;  <pr>&#x3066;&#x3093;&#x306D;&#x3093;&#x3044;&#x308D;&#x3057;&#x3083;&#x3057;&#x3093;&#x3058;&#x3085;&#x3064;</pr></trd>
  <trdgrp lng="nl">
    <trd>chromofotografie</trd>,
    <trd>kleurenfotografie</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>fotografia barwna</trd>,
    <trd>chromofotografia</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>cromofotografia</trd>,
    <trd>fotocromia</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_c;&c_v;&c_je;&c_t;&c_n;&c_a;&c_ja; &c_f;&c_o;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>,
    <trd>&c_h;&c_r;&c_o;&c_m;&c_o;&c_f;&c_o;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="uk">&c_h;&c_r;&#x43E;&#x43C;&#x43E;&c_f;&#x43E;&c_t;&#x43E;&c_g;&c_r;&c_a;&c_f;&c_ib;&#x44F;</trd>
</drv>
