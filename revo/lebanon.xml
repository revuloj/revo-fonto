<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: lebanon.xml,v 1.7 2026/02/07 02:50:35 afido Exp $">
    <kap>
      <rad>Lebanon</rad>/o
      <fnt><bib>MT</bib></fnt>
    </kap>

<drv mrk="lebanon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="lebanon.0o.montaro">
    <dif>
      <ref tip="lst" lst="voko:montaroj" cel="mont.0aro">Montaro</ref> en
      <ref tip="malprt" cel="liban.L0o">Libano</ref>, etendi&gcirc;anta 160&#x202F;km
      la&ubreve;longe de la mediteranea bordo:
      <ekz>
        li sendis ilin sur <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 5:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        supreniru sur <tld/>on kaj kriu
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 22:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        granda aglo <klr>[&#8230;]</klr> venis sur <tld/>on kaj
        prenis pinton de <ref cel="cedr.0o">cedro</ref>
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 17:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="l"/>a cedro, <klr>[&#8230;]</klr> hejma en la montaroj de la Mediteranea
        regiono
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=422991">Lebanona cedro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_L;&c_ib;&c_v;&c_a;&c_n; <klr>(&c_g;&c_o;&c_r;&c_n;&c_y; &c_h;&c_r;&c_y;&c_b;&c_je;&c_t;)</klr></trd>
    <trd lng="de">Libanongebirge</trd>
    <trd lng="en">Mount Lebanon</trd>
    <trd lng="fr">Mont Liban</trd>
    <trd lng="pl">G&oacute;ry <ind>Liban</ind> {pl}</trd>
    <trd lng="zh">&#x9ECE;&#x5DF4;&#x5AE9; <pr>L&#xED;b&amacron;n&#xE8;n</pr></trd>
</snc>
  <trd lng="ja">&#x30EC;&#x30D0;&#x30CE;&#x30F3;&#x5C71;&#33032; 
  <pr>&#x308C;&#12400;&#x306E;&#12435;&#12373;&#12435;&#x307F;&#12419;&#x304F;</pr></trd>
  <trd lng="uk">&c_g;&#x43E;&c_r;&c_i; &#x41B;&c_ib;&c_v;&c_a;&#x43D;</trd>
</drv>
</art>

