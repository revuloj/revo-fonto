<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fibul.xml,v 1.23 2023/11/09 20:48:32 revo Exp $">
<kap>
  <rad>fibul</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fibul.0o">
  <kap><tld/>o</kap>
  <snc mrk="fibul.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Prahistoria a&ubreve; antikva <ref tip="super" cel="brocx.0o.JUV">bro&ccirc;o</ref>, iom granda,
      per kiu oni kuntenadis vestojn:
      <ekz>
        kun sia brakringo el vera oro kaj sia juvela <tld/>o
        en la formo de eta Kupido, &ccirc;u &scirc;i ne estis tre
        speciala persono?
        <fnt><aut>A. L&ouml;wenstein</aut>, <vrk>La &scirc;tona urbo</vrk>, 
        <lok>1999</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Fibel</trd>
    <trd lng="en">fibula<klr tip="ind"> (brooch)</klr></trd>
    <trd lng="fr">fibule<klr tip="ind"> (broche)</klr></trd>
    <trd lng="hu">fibula</trd>
    <trd lng="it">fibula</trd>
    <trd lng="nl">fibula</trd>
    <trd lng="pl">fibula</trd>
    <trd lng="pt">f&iacute;bula</trd>
    <trd lng="ru">&c_f;&c_i;&c_b;&c_u;&c_l;&c_a;</trd>
  </snc>
  <snc mrk="fibul.0o.pinglo">
    <uzo tip="stl">RAR</uzo>
    <ref tip="dif" cel="pingl.sendangxera0o.HEJM">sendan&gcirc;era
    pinglo</ref>
    <trdgrp lng="de">
     <trd>Spange</trd>,
     <trd>Heftel</trd>,
     <trd>Sicherheitsnadel</trd></trdgrp>
    <trd lng="en">safety pin</trd>
    <trdgrp lng="fr">
      <trd>&eacute;pingle &agrave; nourrice</trd>,
      <trd>&eacute;pingle de s&ucirc;ret&eacute;</trd></trdgrp> 
    <trd lng="hu">biztost&udblac;</trd>
    <trdgrp lng="it">
      <trd><ind>spilla</ind> da balia</trd>,
      <trd><ind>spilla</ind> di sicurezza</trd>
    </trdgrp>
    <trd lng="nl">veiligheidsspeld</trd>
    <trd lng="pl">agrafka</trd>
    <trdgrp lng="pt">
      <trd>alfinete de seguran&ccedil;a</trd>,
      <trd>alfinete de fralda</trd>
    </trdgrp>
    <trd lng="ru">&c_a;&c_n;&c_g;&c_l;&c_i;&c_j;&c_s;&c_k;&c_a;&c_ja;
    &c_b;&c_u;&c_l;&c_a;&c_v;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="fibul.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      <ref tip="super" cel="ost.0o">Osto</ref> trovi&gcirc;anta
      kontra&ubreve; la <ref cel="tibi.0o">tibio</ref>, &ccirc;e la
      malanta&ubreve;a parto de la <ref cel="krur.0o.genuo_gxis_piedo">kruro</ref>.     
    </dif>
    <trd lng="de">Wadenbein</trd>
    <trd lng="en">fibula</trd>
    <trdgrp lng="fr">
      <trd>fibula</trd>,
      <trd>p&eacute;ron&eacute;</trd></trdgrp> 
    <trd lng="hu">sz&aacute;rkapocs</trd>
    <trdgrp lng="it">
      <trd>fibula</trd>,
      <trd>perone</trd>
    </trdgrp>
    <trd lng="nl">kuitbeen</trd>
    <trd lng="pl">ko&sacute;&cacute; strza&lstroke;kowa</trd>
    <trdgrp lng="pt">
      <trd>f&iacute;bula</trd>,
      <trd>per&ocirc;nio</trd>
    </trdgrp>
    <trd lng="ru">&c_m;&c_a;&c_l;&c_a;&c_ja; &c_b;&c_je;&c_r;&c_c;&c_o;&c_v;&c_a;&c_ja;
    &c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
    <trd lng="zh">&#x8153;&#x9AA8; <pr>f&#xE9;ig&#x1D4;</pr></trd>
</snc>
  <trdgrp lng="cs">
    <trd>prehistorick&aacute; spona</trd>,
    <trd>p&rcaron;ezka</trd>,
    <trd>sp&iacute;nac&iacute; &scaron;pendl&iacute;k</trd>,
    <trd>zav&iacute;rac&iacute; &scaron;pendl&iacute;k</trd>
  </trdgrp>
  <trd lng="en">fibula<klr tip="ind"> (bone)</klr></trd>
  <trd lng="ja">&#x8153;&#x9AA8;  <pr>&#x3072;&#x3053;&#x3064;</pr></trd>
  <trdgrp lng="sk">
    <trd>spona</trd>,
    <trd>uzatv&aacute;rac&iacute; &scaron;pendl&iacute;k</trd>
  </trdgrp>
  <trd lng="uk">&c_m;&c_a;&c_l;&c_a; &c_g;&c_o;&c_m;&c_ib;&c_l;&c_k;&c_o;&c_v;&c_a; &c_k;&c_ib;&c_s;&c_t;&c_k;&c_a;</trd>
</drv>
</art>
