<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>fibul</rad>/o
</kap>
<drv mrk="fibul.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Prahistoria a&ubreve; antikva bro&ccirc;o. 
    </dif>
    <trd lng="de">Fibel</trd>
    <trd lng="en">fibula</trd>
    <trd lng="fr">fibule<klr tip="ind"> (broche)</klr></trd>
    <trd lng="hu">fibula</trd>
    <trd lng="nl">fibula</trd>
    <trd lng="pl">zapinka</trd>
    <trd lng="ru">&c_f;&c_i;&c_b;&c_u;&c_l;&c_a;</trd>
  </snc>
  <snc num="2">
    <dif>
      Arkforma fermebla pinglo kun &scirc;irmita pinto.
    </dif>
    <refgrp tip="vid">
      <ref cel="agraf.0o">agrafo</ref>.
    </refgrp>
    <trdgrp lng="de">
     <trd>Spange</trd>,
     <trd>Heftel</trd>,
     <trd>Sicherheitsnadel</trd></trdgrp>
    <trd lng="en">safety pin</trd>
    <trdgrp lng="fr">
      <trd>&eacute;pingle &agrave; nourrice</trd>,
      <trd>&eacute;pingle de s&ucirc;ret&eacute;</trd></trdgrp> 
    <trd lng="hu">biztost&udblac;</trd>
    <trd lng="nl">veiligheidsspeld</trd>
    <trd lng="pl">agrafka</trd>
    <trd lng="ru">&c_a;&c_n;&c_g;&c_l;&c_i;&c_j;&c_s;&c_k;&c_a;&c_ja; &c_b;&c_u;&c_l;&c_a;&c_v;&c_k;&c_a;</trd>
  </snc>
  <snc num="3">
    <uzo tip="fak">ANA</uzo>
    <dif>
      <ref tip="super" cel="ost.0o">Osto</ref> trovi&gcirc;anta kontra&ubreve; la <ref cel="tibi.0o">tibio</ref>, &ccirc;e la malanta&ubreve;a parto de la <ref cel="krur.0o.gambo">kruro</ref>.  
    </dif>
    <trd lng="de">Wadenbein</trd>
    <trdgrp lng="fr">
      <trd>fibula</trd>,
      <trd>p&eacute;ron&eacute;</trd></trdgrp> 
    <trd lng="hu">sz&aacute;rkapocs</trd>
    <trd lng="nl">kuitbeen</trd>
    <trd lng="pl">ko&sacute;&cacute; strza&lstroke;kowa</trd>
    <trd lng="ru">&c_m;&c_a;&c_l;&c_a;&c_ja; &c_b;&c_je;&c_r;&c_c;&c_o;&c_v;&c_a;&c_ja; &c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="en">fibula</trd>
</drv>
</art>
