<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gej</rad>/o
</kap>
<drv mrk="gej.0o">    
  <kap><tld/>o</kap>
  <snc mrk="gej.0o.vir">
    <dif>
      Viro kiu estas sekse kaj emocie altirata &ccirc;efe de aliaj viroj;
      samseksama viro:
      <ekz>
        kiajn &scirc;ancojn alportas PACS al ni, lesboj kaj <tld/>oj?
        <fnt>
          <lok>Forumo, membrogazeto de LSG, 1/2000</lok>.
        </fnt>
      </ekz>
    </dif>
    <ref tip="vid" cel="lesb.0o">lesbo</ref> 
    <trd lng="ru">&c_g;&c_je;&c_j;</trd>
   <trdgrp lng="de">
    <trd>Schwuler</trd>,
    <trd>Tunte</trd></trdgrp>
  </snc>
  <snc mrk="gej.0o.amb">
    <dif>
      Persono kiu estas sekse kaj emocie altirata &ccirc;efe de personoj
      de la sama sekso.
    </dif>
    <ref tip="sin" cel="seks.sam0emulo">samseksamulo, samseksemulo</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Homosexueller</trd>,
    <trd>Homo</trd></trdgrp>
  <trdgrp lng="nl">
    <trd>homoseksueel</trd>,
    <trd>homo</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>homossexual</trd>,
    <trd>guei</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_o;&c_m;&c_o;&c_s;&c_je;&c_k;&c_s;&c_u;&c_a;&c_l;&c_i;&c_s;&c_t;</trd>
</drv>

<drv mrk="gej.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>oj:
      <ekz>
        batali kontra&ubreve; anta&ubreve;ju&gcirc;oj kune kun gefratoj <tld/>aj kaj lesbaj
        estus granda honoro por mi
        <fnt>
          <lok>Forumo, membrogazeto de LSG, 2/2000</lok>;
        </fnt>
      </ekz>
      <ekz>
        kion pensi pri la ankora&ubreve;a diskriminacio kontra&ubreve; <tld/>aj kaj lesbaj
        fiduloj? 
        <fnt>
          <aut>Gerrit Berveling</aut>:
          <vrk><url ref="&Monato;/006823.html">Tro senkosta pentemo</url></vrk>, 
          <bib>Monato</bib>, 
          <lok>n-ro 08/2000</lok>.
        </fnt>
      </ekz>
    </dif>
    <ref tip="vid" cel="lesb.0a">lesba</ref>   
  </snc>
   <trdgrp lng="de">
    <trd>schwul</trd>,
    <trd>homosexuell</trd></trdgrp>
  <trd lng="nl">homoseksueel</trd>
  <trd lng="pt">homossexual</trd>
</drv>

</art>

