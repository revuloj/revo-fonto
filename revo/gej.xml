<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gej.xml,v 1.26 2023/05/04 08:31:33 revo Exp $">
<kap>
  <rad>gej</rad>/o
</kap>
<drv mrk="gej.0o">    
  <kap><tld/>o</kap>
  <snc mrk="gej.0o.vir">
    <dif>
      Virseksa <ref tip="super" cel="seks.sam0emulo">samseksemulo</ref>:
      <ekz>
        kiajn &scirc;ancojn alportas PACS al ni, lesboj kaj <tld/>oj?
        <fnt><lok>Forumo, membrogazeto de LSG, 1/2000</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="lesb.l0o">lesbo</ref>
    <trd lng="ca">gai<klr> (subst.)</klr></trd>
    <trdgrp lng="de">
      <trd>Schwuler</trd>,
      <trd>Tunte</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>gay</trd>, 
      <trd>homo</trd>,
      <trd>homosexuel<klr tip="ind"> (subst.)</klr></trd>,
    </trdgrp>
    <trd lng="ja">&#x30B2;&#x30A4; [<ind>&#12370;&#12356;</ind>]</trd>
    <trd lng="ru">&c_g;&c_je;&c_j;</trd>
  </snc>
  <snc mrk="gej.0o.amb">
    <ref tip="dif" cel="seks.sam0emulo">samseksemulo</ref>
    <rim>
      La vorto <tld/>o dekomence montris nur virojn, sed sekvante la
      ekzemplon de multaj similaj vortoj, inkluzive de &ccirc;iuj deriva&jcirc;oj
      per -ul, nenio teorie malhelpas vastigi &gcirc;in al amba&ubreve; seksoj.
    </rim>
    <trd lng="ca">homosexual<klr> (subst.)</klr></trd>
    <trdgrp lng="fr">
      <trd>gay</trd>, 
      <trd>homosexuel<klr tip="ind"> (subst.)</klr></trd>,
      <trd>homosexuelle<klr tip="ind"> (subst.)</klr></trd>,
    </trdgrp>
  </snc>
  <trd lng="be">&c_g;&c_o;&c_m;&c_a;&c_s;&c_e;&c_k;&c_s;&c_u;&c_a;&c_l;&c_ib;&c_s;&c_t;</trd>
  <trd lng="cs">homosexu&aacute;l</trd>
  <trdgrp lng="de">
    <trd>Homosexueller</trd>,
    <trd>Homo</trd>
  </trdgrp>
  <trd lng="en">gay<klr tip="ind"> (noun)</klr></trd>
  <trdgrp lng="es">
    <trd>homosexual</trd>,
    <trd>gay</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>homoszexu&aacute;lis</trd>,
    <trd>homokos</trd>,
    <trd>meleg<klr tip="ind"> (homokos)</klr></trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>gay</trd>,
    <trd>homo</trd>,
    <trd>homoseksual</trd>
  </trdgrp>  
  <trdgrp lng="it">
    <trd>omosessuale<klr tip="ind"> (sost.)</klr></trd>,
    <trd>gay<klr tip="ind"> (sost.)</klr></trd>
  </trdgrp> 
  <trd lng="ja">&#x540C;&#24615;&#x611B;&#32773; [<ind>&#12393;&#12358;&#x305B;&#12356;&#12354;&#12356;&#12375;&#12419;</ind>]</trd>
  <trdgrp lng="nl">
    <trd>homoseksueel</trd>,
    <trd>homo</trd>,
    <trd>gay</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>homossexual</trd>,
    <trd>gay</trd>,
    <trd>guei</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_o;&c_m;&c_o;&c_s;&c_je;&c_k;&c_s;&c_u;&c_a;&c_l;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">homosexu&aacute;l</trd>
  <trdgrp lng="uk">
    <trd>&c_gu;&c_je;&c_j;</trd>,
    <trd>&c_g;&#x43E;&#x43C;&#x43E;&c_s;&c_je;&#x43A;&c_s;&c_u;&c_a;&#x43B;&c_ib;&c_s;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="gej.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>oj, <ref cel="seks.sam0ema">samseksema</ref>:
      <ekz>
        batali kontra&ubreve; anta&ubreve;ju&gcirc;oj kune kun gefratoj 
        <tld/>aj kaj lesbaj estus granda honoro por mi
        <fnt>
          <lok>Forumo, membrogazeto de LSG, 2/2000</lok>
        </fnt>;
      </ekz>
      <ekz>
        kion pensi pri la ankora&ubreve;a diskriminacio kontra&ubreve; <tld/>aj 
        kaj lesbaj fiduloj? 
        <fnt>
          <aut>Gerrit Berveling</aut>:
          <vrk><url ref="&Monato;/2000/006823.html">Tro senkosta
          pentemo</url></vrk>, 
          <bib>Monato</bib>, 
          <lok>2000/08</lok>
        </fnt>;
      </ekz>
      <ekz>
        la t.n. <tld/>a vivstilo.
      </ekz>
    </dif>
    <ref tip="vid" cel="lesb.l0a">lesba</ref>
  </snc>
  <trd lng="be">&c_g;&c_o;&c_m;&c_a;&c_s;&c_e;&c_s;&c_k;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trd lng="bo">&#xF55;&#xF7C;&#xF0B;&#xF60;&#xF47;&#xF74;&#xF42;&#xF0B;</trd>
  <trdgrp lng="ca">
    <trd>gai<klr> (adj.)</klr></trd>,
    <trd>homosexual<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="cs">homosexu&aacute;ln&iacute;</trd>
  <trdgrp lng="de">
    <trd>schwul</trd>,
    <trd>homosexuell</trd>
  </trdgrp>
  <trd lng="en">gay<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="es">homosexual</trd>
  <trdgrp lng="fr">
    <trd>gay</trd>
    <trd>homosexuel<klr tip="ind"> (adj.)</klr></trd>,
    <trd>homosexuelle<klr tip="ind"> (adj.)</klr></trd>,
  </trdgrp>
  <trdgrp lng="hu">
    <trd>homoszexu&aacute;lis</trd>,
    <trd>homokos</trd>,
    <trd>meleg<klr tip="ind"> (homokos)</klr></trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>gay</trd>,
    <trd>homo</trd>,
    <trd>homoseksual</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>omosessuale<klr tip="ind"> (agg.)</klr></trd>,
    <trd>gay<klr tip="ind"> (agg.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x540C;&#24615;&#x611B;&#x306E; [<ind>&#12393;&#12358;&#x305B;&#12356;&#12354;&#12356;&#x306E;</ind>]</trd>,
    <trd>&#x540C;&#24615;&#x611B;&#32773;&#x306E; [<ind>&#12393;&#12358;&#x305B;&#12356;&#12354;&#12356;&#12375;&#12419;&#x306E;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>homoseksueel</trd>
    <trd>gay</trd>
  </trdgrp>
  <trd lng="pt">homossexual</trd>
  <trd lng="sk">homosexu&aacute;lny</trd>
  <trd lng="uk">&#x43F;&c_r;&c_i;&c_z;&#x43D;&c_a;&c_ch;&c_je;&#x43D;&c_i;&c_j; &c_d;&#x43B;&#x44F; &c_gu;&c_je;&c_ji;&c_v;</trd>
</drv>

<drv mrk="gej.mal0o">    
  <kap>mal<tld/>o</kap>
  <snc>
    <ref tip="dif" cel="seks.ali0emulo">aliseksemulo</ref>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_t;&c_e;&c_r;&c_a;&c_s;&c_e;&c_k;&c_s;&c_u;&c_a;&c_l;</trd>
  <trd lng="en">straight</trd>
  <trdgrp lng="fr">
    <trd>h&eacute;t&eacute;ro</trd>,
    <trd>h&eacute;t&eacute;rosexuel<klr tip="ind"> (subst.)</klr></trd>,
    <trd>h&eacute;t&eacute;rosexuelle<klr tip="ind"> (subst.)</klr></trd>,
  </trdgrp>
  <trd lng="nl">hetero</trd>
</drv>

<drv mrk="gej.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Rilata al mal<tld/>oj, <ref cel="seks.ali0ema">aliseksema</ref>.
    </dif>
  </snc>  
  <trd lng="be">&c_g;&c_je;&c_t;&c_e;&c_r;&c_a;&c_s;&c_e;&c_k;&c_s;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trd lng="en">straight</trd>
  <trdgrp lng="fr">
    <trd>h&eacute;t&eacute;ro</trd>,
    <trd>h&eacute;t&eacute;rosexuel<klr tip="ind"> (adj.)</klr></trd>,
    <trd>h&eacute;t&eacute;rosexuelle<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
</drv>


 <drv mrk="gej.0oj_lesboj_ambauxseksemuloj_kaj_transgenruloj">
  <kap>
    <tld/>oj, lesboj, amba&ubreve;seksemuloj kaj transgenruloj
  </kap>
    <mlg>GLAT</mlg>  <snc>
    <dif>
      La komunumo de
      <ref cel="gej.0o">gejoj</ref>, <ref cel="lesb.l0o">lesboj</ref>,
      <ref cel="seks.ambaux0emulo">amba&ubreve;seksemuloj</ref> kaj <ref
      cel="genr.trans0ulo">transgenruloj</ref>:
      <ekz>
        la videon ni faris dum ni &ccirc;eestis Interlingvistikan Simpozion en
        Poznan, kaj poste Arkones, kie ni faris sukcesoplenan
        diskutrondon pri GLAT
        <fnt>
          <vrk>Pozitiva rondo</vrk>,
          <lok><url ref="https://egalecen.org/2014/09/30/pozitiva-rondo/">Egalecen</url>,
          2014-09-30</lok>
        </fnt>;
      </ekz>
      <ekz>
        subtenado de la rajtoj de GLAT-aj personoj estu tutmonde
        la&ubreve;data
        <fnt>
          <vrk>Naciismo kaj GLAT-aj rajtoj</vrk>
          <lok><url ref="&Viki;/Naciismo_kaj_GLAT-aj_rajtoj">Vikipedio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        por handikapaj GLAT-uloj &gcirc;i tre gravas, &ccirc;ar ili povas viziti
        sian partneron en malsanulejo pli facile
        <fnt>
          <aut>Rowland Bonkorpa</aut>:
          <vrk>Gejedzi&gcirc;o</vrk>.
          <lok>&leftquot;Kontakto&rightquot;, n-ro 268 (2015:04). P. 14a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_L;&c_G;&c_B;&c_T;</trd>
  <trd lng="en">LGBT</trd>
  <trdgrp lng="es">
    <trd>LGBT</trd>,
    <trd>LGTB</trd>
  </trdgrp>
  <trd lng="fr">LGBT</trd>
  <trdgrp lng="nl">
    <trd>lgbt</trd>,
    <trd>lhbt</trd>
  </trdgrp>
</drv>



</art>

