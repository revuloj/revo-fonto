<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gej</rad>/o
</kap>
<drv mrk="gej.0o">    
  <kap><tld/>o</kap>
  <snc mrk="gej.0o.vir">
    <dif>
      <!-- Viro, kiu estas sekse kaj emocie altirata &ccirc;efe de aliaj
      viroj;
      samseksama viro: -->
      <ref cel="seks.sam0ema">Samseksama</ref> viro:
      <ekz>
        kiajn &scirc;ancojn alportas PACS al ni, lesboj kaj <tld/>oj?
        <fnt><lok>Forumo, membrogazeto de LSG, 1/2000</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="lesb.l0o">lesbo</ref>
    <trd lng="ca">gai<klr> (subst.)</klr></trd>
    <trdgrp lng="de">
      <trd>Schwuler</trd>,
      <trd>Tunte</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>gay</trd>, 
      <trd>homo</trd>,
      <trd>homosexuel<klr tip="ind"> (subst.)</klr></trd>,
    </trdgrp>
  <trd lng="ja">&#12466;&#12452; [<ind>&#12370;&#12356;</ind>]</trd>
   <trd lng="ru">&c_g;&c_je;&c_j;</trd>
  </snc>
  <snc mrk="gej.0o.amb">
    <dif>
      <!-- Persono, kiu estas sekse kaj emocie altirata &ccirc;efe de
      personoj de la sama sekso. -->
      <ref cel="seks.sam0ema">Samseksama</ref> persono.
    </dif>
    <rim>
      La vorto <tld/>o dekomence montris nur virojn, sed sekvante la
      ekzemplon
      de multaj similaj vortoj, inkluzive de &ccirc;iuj deriva&jcirc;oj per
      -ul, nenio teorie malhelpas vastigi &gcirc;in al amba&ubreve; seksoj.
    </rim>
    <trd lng="ca">homosexual<klr> (subst.)</klr></trd>
    <trdgrp lng="fr">
      <trd>gay</trd>, 
      <trd>homosexuel<klr tip="ind"> (subst.)</klr></trd>,
      <trd>homosexuelle<klr tip="ind"> (subst.)</klr></trd>,
   </trdgrp>
  </snc>
  <ref tip="sin" cel="seks.sam0emulo">samseksamulo, samseksemulo.</ref>
  <ref tip="sin" cel="seks.sam0emulino">samseksamulino,
  samseksemulino.</ref>
  <trd lng="be">&c_g;&c_o;&c_m;&c_a;&c_s;&c_e;&c_k;&c_s;&c_u;&c_a;&c_l;&c_ib;&c_s;&c_t;</trd>
  <trd lng="cs">homosexu&aacute;l</trd>
  <trdgrp lng="de">
    <trd>Homosexueller</trd>,
    <trd>Homo</trd>
  </trdgrp>
  <trd lng="en">gay<klr tip="ind"> (noun)</klr></trd>
  <trdgrp lng="es">
    <trd>homosexual</trd>,
    <trd>gay</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>homoszexu&aacute;lis</trd>,
    <trd>homokos</trd>,
    <trd>meleg<klr tip="ind"> (homokos)</klr></trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>gay</trd>,
    <trd>homo</trd>,
    <trd>homoseksual</trd>
  </trdgrp>  
  <trdgrp lng="it">
    <trd>omosessuale<klr tip="ind"> (sost.)</klr></trd>,
    <trd>gay<klr tip="ind"> (sost.)</klr></trd>
  </trdgrp> 
  <trd lng="ja">&#21516;&#24615;&#24859;&#32773; [<ind>&#12393;&#12358;&#12379;&#12356;&#12354;&#12356;&#12375;&#12419;</ind>]</trd>
  <trdgrp lng="nl">
    <trd>homoseksueel</trd>,
    <trd>homo</trd>,
    <trd>gay</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>homossexual</trd>,
    <trd>gay</trd>,
    <trd>guei</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_o;&c_m;&c_o;&c_s;&c_je;&c_k;&c_s;&c_u;&c_a;&c_l;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">homosexu&aacute;l</trd>
</drv>

<drv mrk="gej.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>oj, samseksama:
      <ekz>
        batali kontra&ubreve; anta&ubreve;ju&gcirc;oj kune kun gefratoj 
        <tld/>aj kaj lesbaj estus granda honoro por mi
        <fnt>
          <lok>Forumo, membrogazeto de LSG, 2/2000</lok>
        </fnt>;
      </ekz>
      <ekz>
        kion pensi pri la ankora&ubreve;a diskriminacio kontra&ubreve; <tld/>aj 
        kaj lesbaj fiduloj? 
        <fnt>
          <aut>Gerrit Berveling</aut>:
          <vrk><url ref="&Monato;/006823.html">Tro senkosta
          pentemo</url></vrk>, 
          <bib>Monato</bib>, 
          <lok>n-ro 08/2000</lok>
        </fnt>;
      </ekz>
      <ekz>
        la t.n. <tld/>a vivstilo.
      </ekz>
    </dif>
    <ref tip="vid" cel="lesb.l0a">lesba.</ref>
  </snc>
  <trd lng="be">&c_g;&c_o;&c_m;&c_a;&c_s;&c_e;&c_s;&c_k;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trdgrp lng="ca">
    <trd>gai<klr> (adj.)</klr></trd>,
    <trd>homosexual<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="cs">homosexu&aacute;ln&iacute;</trd>
  <trdgrp lng="de">
    <trd>schwul</trd>,
    <trd>homosexuell</trd>
  </trdgrp>
  <trd lng="en">gay<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="es">homosexual</trd>
  <trdgrp lng="fr">
    <trd>gay</trd>
    <trd>homosexuel<klr tip="ind"> (adj.)</klr></trd>,
    <trd>homosexuelle<klr tip="ind"> (adj.)</klr></trd>,
  </trdgrp>
  <trdgrp lng="hu">
    <trd>homoszexu&aacute;lis</trd>,
    <trd>homokos</trd>,
    <trd>meleg<klr tip="ind"> (homokos)</klr></trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>gay</trd>,
    <trd>homo</trd>,
    <trd>homoseksual</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>omosessuale<klr tip="ind"> (agg.)</klr></trd>,
    <trd>gay<klr tip="ind"> (agg.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#21516;&#24615;&#24859;&#12398; [<ind>&#12393;&#12358;&#12379;&#12356;&#12354;&#12356;&#12398;</ind>]</trd>,
    <trd>&#21516;&#24615;&#24859;&#32773;&#12398; [<ind>&#12393;&#12358;&#12379;&#12356;&#12354;&#12356;&#12375;&#12419;&#12398;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>homoseksueel</trd>
    <trd>gay</trd>
  </trdgrp>
  <trd lng="pt">homossexual</trd>
  <trd lng="sk">homosexu&aacute;lny</trd>
</drv>

<drv mrk="gej.mal0o">    
  <kap>mal<tld/>o</kap>
  <snc mrk="gej.mal0o.vir">
    <dif>
      <ref cel="seks.ali0ema">Aliseksama</ref> viro:
    </dif>
  <trdgrp lng="fr">
    <trd>h&eacute;t&eacute;ro</trd>,
    <trd>h&eacute;t&eacute;rosexuel<klr tip="ind"> (subst.)</klr></trd>,
  </trdgrp>
  </snc>
  <snc mrk="gej.mal0o.amb">
    <dif>
      <ref cel="seks.ali0ema">Aliseksama</ref> persono.
    </dif>
    <rim>
      La vorto <tld/>o dekomence montris nur virojn, sed sekvante la
      ekzemplon
      de multaj similaj vortoj, inkluzive de &ccirc;iuj deriva&jcirc;oj per
      -ul, nenio teorie malhelpas vastigi &gcirc;in al amba&ubreve; seksoj.
    </rim>
  <trdgrp lng="fr">
    <trd>h&eacute;t&eacute;ro</trd>,
    <trd>h&eacute;t&eacute;rosexuel<klr tip="ind"> (subst.)</klr></trd>,
    <trd>h&eacute;t&eacute;rosexuelle<klr tip="ind"> (subst.)</klr></trd>,
  </trdgrp>
  </snc>
  <ref tip="sin" cel="seks.ali0emulo">aliseksamulo, aliseksemulo.</ref>
  <ref tip="sin" cel="seks.ali0emulino">aliseksamulino,
  aliseksemulino.</ref>
  <trd lng="en">straight</trd>
</drv>

<drv mrk="gej.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Rilata al mal<tld/>oj, <ref cel="seks.ali0ema">aliseksama</ref>.
    </dif>
  </snc>  
  <trd lng="en">straight</trd>
  <trdgrp lng="fr">
    <trd>h&eacute;t&eacute;ro</trd>,
    <trd>h&eacute;t&eacute;rosexuel<klr tip="ind"> (adj.)</klr></trd>,
    <trd>h&eacute;t&eacute;rosexuelle<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
</drv>

</art>

