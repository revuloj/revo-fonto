<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: konvers.xml,v 1.4 2023/08/30 13:29:35 revo Exp $">
<kap>
  <rad>konvers</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="konvers.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="konvers.0i.KOMUNE">
    <dif>
      <ref tip="dif" cel="konver2.0i.KOMUNE">Konversacii</ref>:
      <ekz>
        lastatempe, kiam li <tld/>is kun mi, lia parolo &ccirc;iam tu&scirc;is <nom>Qin</nom>
        <fnt><bib>LaFamilio</bib>, <lok>&ccirc;apitro 11a, p. 122a</lok></fnt>;
      </ekz>
      <ekz>
        Petro Damiano kvalifikis la mona&hcirc;ejan &ccirc;elon interparola &ccirc;ambro kie Dio <tld/>as
        kun la homoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=421947"
              >Petro Damiano</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>sich <ind>unterhalten</ind></trd>,
      <trd>konversieren</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_t;&c_a;&c_r;&c_y;&c_c;&c_mol;</trd>
  <trd lng="fr">s'<ind>entretenir</ind></trd>
  <trd lng="pl">konwersowa&cacute;</trd>
  <trd lng="ro">s&abreve; vorbe&#537;ti</trd>
</drv>
</art>

