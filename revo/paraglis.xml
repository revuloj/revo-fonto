<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: paraglis.xml,v 1.8 2023/03/21 08:32:19 revo Exp $">
<kap>
  <rad>paraglis</rad>/i
</kap>
<drv mrk="paraglis.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="paraglis.0ado.SPO">
    <uzo tip="fak">SPO</uzo>
    <ref tip="dif" cel="parasx.glis0ado.AVI">glispara&scirc;utado</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_p;&c_l;&c_ja;&c_n;&c_e;&c_r;&c_y;&c_z;&c_m;</trd>
  <trd lng="ca">parapent <klr>(esport)</klr></trd>
  <trdgrp lng="de">
    <trd>Paragliding</trd>,
    <trd>Gleitschirmfliegen</trd>
  </trdgrp>
  <trd lng="fr">parapente<klr> (sport)</klr></trd>
  <trd lng="it">parapendio</trd>
  <trd lng="pl">paralotniarstwo</trd>
  <trd lng="pt">parapente<klr> (esporte)</klr></trd>
</drv>

<drv mrk="paraglis.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="paraglis.0ilo.SPO">
    <uzo tip="fak">AVI</uzo>
    <dif>
      <ref tip="dif" cel="parasx.glis0o.AVI">Glispara&scirc;uto</ref>:
      <ekz>
        la unua <tld/>ilo estas la t.n. &leftquot;<nac>sailwing</nac>&rightquot; konstruita de
        <nom>David Barish</nom> en 1965
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;paraglisado&rightquot;,
        2005-12-02</lok></fnt>;
      </ekz>
      <ekz>
        en jaro 2000 mi voja&gcirc;is al &Ccirc;inio por flugi per <tld/>ilo
        <fnt>
        <aut>&#51312;&#50896;&#49437; (Vintro)</aut>,
        <vrk>Esperanto, kiu malfermis la novan horizonton de mia
        vivo</vrk>,
        <lok><url ref="http://mobigen.com/~hiongun/LA/">La Lanterno
        Azia,
        2005-07-07</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_p;&c_l;&c_ja;&c_n;</trd>
  <trd lng="ca">parapent</trd>
  <trd lng="de">Gleitschirm</trd>
  <trd lng="fr">parapente<klr> (v&eacute;hicule)</klr></trd>
  <trd lng="it">parapendio</trd>
  <trd lng="pl">paralotnia</trd>
  <trd lng="pt">parapente</trd>
</drv>
</art>

