<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kontor.xml,v 1.25 2023/10/18 17:23:24 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kontor</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kontor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kontor.0o.EKON">
    <dif>
      <ref tip="super" cel="ofic.0ejo">Oficejo</ref>, kiu peras al
      klientoj la servojn de komerca, industria <ref tip="malprt" cel="kompan.0o.afero">kompanio</ref>:
      <ekz>
        la <tld/>o de la fabriko;
      </ekz>
      <ekz>
        en sabato &ndash; tre maloportune, &ccirc;ar &ccirc;iuj <tld/>oj kaj anka&ubreve;
        informejoj estas fermitaj
        <fnt><aut>nikst</aut>, <vrk>Adresoj de Ortodoksa eklezio en
        RU/USA</vrk>, <lok>soc.culture.esperanto,
        1996-02-13</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="agent.0ejo">agentejo</ref>,
      <ref cel="faktor1.0o">faktorio</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_n;&c_t;&c_o;&c_r;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_c;&c_y;&c_l;&c_ja;&c_r;&c_y;&c_ja;</trd>,
    <trd>&c_b;&c_ju;&c_r;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kancel&aacute;&rcaron;</trd>,
    <trd>konto&aacute;r</trd>,
    <trd>p&iacute;s&aacute;rna</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Handelskontor</trd>,
    <trd>Fabrikb&uuml;ro</trd>,
    <trd>B&uuml;ro</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>countinghouse</trd>,
    <trd>bureau</trd>,
    <trd><klr>accounting</klr> office</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>agence</trd>,
    <trd>succursale</trd>
  </trdgrp>
  <trd lng="hu">iroda</trd>
  <trdgrp lng="ja">
    <trd>&#x4E8B;&#x52D9;&#x5BA4;  <pr>&#x3058;&#x3080;&#x3057;&#x3064;</pr></trd>,
    <trd>&#x5E33;&#x5834;  <pr>&#x3061;&#x3087;&#x3046;&#x3070;</pr></trd>
  </trdgrp>
  <trd lng="nl">kantoor</trd>
  <trd lng="pl">kantor</trd>     
  <trd lng="ro">birou</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_n;&c_t;&c_o;&c_r;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_c;&c_je;&c_l;&c_ja;&c_r;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kancel&aacute;ria</trd>,
    <trd>pis&aacute;re&ncaron;</trd>
  </trdgrp>
  <trd lng="sv">kontor</trd>
</drv>

<drv mrk="kontor.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="kontor.0isto.EKON">
    <dif>
      Homo, kiu laboras en <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_n;&c_t;&c_o;&c_r;&c_sh;&c_ch;&c_y;&c_k;</trd>,
    <trd>&c_k;&c_l;&c_je;&c_r;&c_k;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_c;&c_y;&c_l;&c_ja;&c_r;&c_y;&c_s;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>koncipient</trd>,
    <trd>konto&aacute;rista</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kontorist</trd>,
    <trd>kaufm&auml;nnischer Angestellter</trd>
  </trdgrp>
  <trd lng="en">clerk</trd>
  <trd lng="fr">employ&eacute; d'agence</trd>
  <trdgrp lng="hu">
    <trd>irodista</trd>,
    <trd>irodai dolgoz&oacute;</trd>
  </trdgrp>
  <trd lng="ja">&#x4E8B;&#x52D9;&#x54E1;  <pr>&#x3058;&#x3080;&#x3044;&#x3093;</pr></trd>
  <trd lng="nl">kantoorbediende</trd>
  <trd lng="pl">biuralista</trd>  
  <trd lng="pt">escrit&oacute;rio</trd>
  <trd lng="ro">secretar</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_n;&c_t;&c_o;&c_r;&c_s;&c_k;&c_i;&c_j; &c_s;&c_l;&c_u;&c_zh;&c_a;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_k;&c_l;&c_je;&c_r;&c_k;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_c;&c_je;&c_l;&c_ja;&c_r;&c_i;&c_s;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">koncipient</trd>
</drv>
</art>
