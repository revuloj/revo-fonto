<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: osak.xml,v 1.5 2022/05/01 17:17:01 afido Exp $">
    <kap>
      <rad>Osak</rad>/o
    </kap>

<drv mrk="osak.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Granda <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      en <ref tip="malprt" cel="japan.0ujo">Japanujo</ref>,
      situanta duonvoje inter <ref cel="toki.0o">Tokio</ref> 
      kaj <ref cel="hirosxim.0o">Hiro&scirc;imo</ref>:
      <ekz>
        <nom>Hundertwasser</nom> projektis <klr>[&#x2026;]</klr> ruba&jcirc;bruligejon kaj pumpstacion en
        <tld/>o, Japanio
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Steloj;monato2/007014.html">A&ubreve;strio: Orfi&gcirc;inta beleco</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en Japanio la urbaj regionoj Tokio, <tld/>o-Kobeo kaj Nagojo proksimi&gcirc;is al 70 %
        <klr>[de la MNP]</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="&Monato;/2004/008325.html">Kvin defioj por &ccirc;inaj urboj</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_O;&c_s;&c_a;&c_k;&c_a;</trd>
    <trd lng="de">Osaka</trd>
    <trd lng="en">Osaka</trd>
    <trd lng="fr">Osaka</trd>
    <trd lng="ja">&#x5927;&#x962A;&#x5E02;</trd>
    <trd lng="pl">Osaka</trd>
    <trd lng="ro">Osaka</trd>
  </snc>
  <trd lng="uk">&#x41E;&#x441;&#x430;&#x43A;&#x430;</trd>
</drv>

  </art>

