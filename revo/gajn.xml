<drv mrk="gajn.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="gajn.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="akir.0i">Akiri</ref> ion bonan per sia penado:
      <ekz>
        <tld/>i monon, sperton, vivrimedojn, ies favoron,
        konfidon, &ccirc;ies korojn;
      </ekz>
      <ekz>
        <klr>[li]</klr>j negocadis per ili kaj <tld/>is pluajn kvin talantojn
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 25:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel profitus homo, se li <tld/>us la tutan mondon kaj perdus sian animon
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 16:26</lok>
        </fnt>?
      </ekz>
      <ekz>
        mi scias bone, ke vi deziras <tld/>i tempon
        <klr>(vd <ref cel="prokra.0i">prokrasti</ref>)</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 2:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>is per la inter&scirc;an&gcirc;o
        <fnt>
          <bib>ElektFab</bib>
          <lok>Feli&ccirc;a Jo&ccirc;jo</lok>
        </fnt>;
      </ekz>
      <ekz>
        per reformado ni <klr>[&#8230;]</klr> <tld/>os absolute nenion
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en Dresden en la 17a de a&ubreve;gusto
          1908</vrk>
        </fnt>;
      </ekz>
      <ekz>
        ri&ccirc;a grafo el Regensburgo <klr>[&#8230;]</klr> <tld/>is proceson pri miliono per la
        ruza&jcirc;oj de sia advokato
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi kredas <tld/>i almena&ubreve; mil dukatojn per tiu &ccirc;i unua eldono
        <fnt>
          <bib>DKM</bib>
          <lok>&Ccirc;apitro LXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        en tiuj du monatoj da plena libereco, oni re<tld/>os la tempon perditan
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        por <tld/>i sian panon, tiu malfeli&ccirc;ulo plenumis la plej humilajn kaj la plej
        lacigajn laborojn
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Deka</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="ating.0i">atingi</ref>,
       <ref cel="spez.en0i">enspezi</ref>,
       <ref cel="konker.0i">konkeri</ref>,
       <ref cel="labor.per0i">perlabori</ref>,
       <ref cel="profit.0i">profiti</ref>,
       <ref cel="ricev.0i">ricevi</ref>,
       <ref cel="rikolt.0i">rikolti</ref>,
       <ref cel="venk.0i.konkurse">venki</ref>
     </refgrp>
    <trd lng="be">&c_z;&c_a;&c_r;&c_a;&c_b;&c_l;&c_ja;&c_c;&c_mol;</trd>
    <trd lng="de">erwerben</trd>
    <trdgrp lng="id">
      <trd><baz>oleh</baz>memperoleh</trd>,
      <trd><baz>dapat</baz>mendapatkan</trd>
    </trdgrp>
    <trd lng="nl">verdienen</trd>
  </snc>
  <snc mrk="gajn.0i.FIG">
    <dif>
      <ref tip="super" cel="akir.0i">Akiri</ref> ion bonan per favoro de la sorto:
      <ekz>
        50&#x202F;000 frankojn <klr>[mi]</klr> gajnis en la loterio
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>!
      </ekz>
      <ekz>
        lia numero eliris el la lotrado kaj <tld/>is la ducentmilfrankan lota&jcirc;on
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Deka</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u mi ne <tld/>is la tutan veton
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>?
      </ekz>
      <ekz>
        pli bone malmulte <tld/>i, ol multe perdi
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu ne riskas, tiu ne <tld/>as
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_y;&c_j;&c_g;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="id"><baz>menang</baz>memenangkan</trd>
    <trdgrp lng="nl">
      <trd>overwinnen</trd>,
      <trd>winnen</trd>
    </trdgrp>
  </snc>
  <trd lng="af">oorwinning</trd>
  <trd lng="am">&#x121B;&#4664;&#4752;&#x134D;</trd>
  <trd lng="ar">&a_A;&a_l;&a_f;&a_w;&a_z;</trd>
