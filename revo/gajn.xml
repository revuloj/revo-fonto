<drv mrk="gajn.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc mrk="gajn.0o.ago">
    <dif>
      Ago <tld/>i:
      <ekz>
        malhonora <tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>Tito 1:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        homoj en tiu batalo ne sciis, por kio kaj kial ili batalis <klr>[kaj ili]</klr>
        ne fari&gcirc;is pli feli&ccirc;aj per la <tld/>o kaj perdo
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Gewinnen</trd>
    <trd lng="hu">nyer&eacute;s</trd>
    <trd lng="id"><baz>oleh</baz>perolehan</trd>
    <trd lng="nl">het <ind>winnen</ind></trd>
    <trd lng="zh">&#x76C8;&#21033; <pr>y&#xED;ngl&#xEC;</pr></trd>
</snc>
  <snc mrk="gajn.0o.gajnitajxo">
    <dif>
      <tld lit="G"/>a&jcirc;o:
      <ekz>
        mi proponus al vi <tld/>on da mil frankoj
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>;
      </ekz>
      <ekz>
        ankora&ubreve; la <tld/>o ne estas en la mano
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        pli bona estas <tld/>o malgranda, ol granda mal<tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Gewinn</trd>,
      <trd>Erwerb</trd>
    </trdgrp>
    <trd lng="hu">nyerem&eacute;ny</trd>
    <trdgrp lng="id">
      <trd><baz>hasil</baz>penghasilan</trd>,
      <trd><baz>dapat</baz>pendapatan</trd>
    </trdgrp>
    <trd lng="nl">winst</trd>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_j;&c_g;&c_r;&c_y;&c_sh;</trd>
  <trd lng="ca">guany</trd>
  <trdgrp lng="cs">
    <trd>v&yacute;hoda</trd>,
    <trd>v&yacute;hra</trd>,
    <trd>zisk</trd>
  </trdgrp>
  <trd lng="es">ganancia</trd>
  <trdgrp lng="fr">
    <trd>gain</trd>,
    <trd>revenu<klr tip="ind"> (gain)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#29554;&#x5F97;  <pr>&#x304B;&#x304F;&#12392;&#x304F;</pr></trd>,
    <trd>&#x52DD;&#12385;  <pr>&#x304C;&#12385;</pr></trd>,
    <trd>&#21033;&#x76CA;  <pr>&#x308A;&#12360;&#x304D;</pr></trd>,
    <trd>&#12418;&#12358;&#12369;</trd>
  </trdgrp>
  <trd lng="ne">&#x91C;&#2368;&#2340;</trd>
  <trd lng="pt">ganho</trd>
  <trd lng="ru">&c_v;&c_y;&c_i;&c_g;&c_r;&c_y;&c_sh;</trd>
  <trdgrp lng="sk">
    <trd>v&yacute;hra</trd>,
    <trd>zisk</trd>
  </trdgrp>
  <trd lng="tr">kazan&ccedil;</trd>
  <trd lng="uk">&c_v;&c_i;&c_g;&c_r;&c_a;&c_sh;</trd>
</drv>

<drv mrk="gajn.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Ne akiri ion esperitan, ne konservi avanta&gcirc;on,
      <ref cel="venk.mal0i">malvenki</ref>:
      <ekz>
        mi mal<tld/>is pli ol cent asojn en morao-ludo
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;APITRO XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        se vi mal<tld/>os, mi estos malfeli&ccirc;a
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        Kristoforo preska&ubreve; &ccirc;iam mal<tld/>is, sed tio ne estis tre grava, la&ubreve; lia opinio
        <fnt>
          <bib>HsT</bib>
          <lok>Sia propra mastro</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="perd.0i">perdi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_j;&c_g;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bo">&t_ba;&t_ra;&t_lata;&t_ga; &t_ba;&t_ra;&t_lata;&t_ga;&t_sa; &t_ba;&t_ra;&t_lata;&t_ga;&t_tsheg;</trd>
  <trd lng="ca">perdre</trd>
  <trdgrp lng="cs">
    <trd>prod&ecaron;lat</trd>,
    <trd>prohr&aacute;t</trd>
  </trdgrp>
  <trd lng="de">verlieren</trd>
  <trd lng="es">perder</trd>
  <trd lng="fr">perdre<klr tip="ind"> (de l'argent, une bataille, au
  jeu,
  etc.)</klr></trd>
  <trd lng="hu">vesz&iacute;t</trd>
  <trdgrp lng="id">
    <trd>kalah</trd>,
    <trd><baz>hilang</baz>kehilangan</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#22833;&#12358;  <pr>&#12358;&#12375;&#x306A;&#12358;</pr></trd>,
    <trd>&#x8CA0;&#12369;&#x308B;  <pr>&#x307E;&#12369;&#x308B;</pr></trd>,
    <trd>&#x640D;&#12434;&#12377;&#x308B;  <pr>&#x305D;&#12435;&#12434;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">verliezen</trd>
  <trd lng="pt">perder</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_i;&c_g;&c_r;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>prehra&tcaron; <klr>(hru)</klr></trd>,
    <trd>strati&tcaron; peniaze v&nbsp;hre</trd>
  </trdgrp>
  <trd lng="tr">kaybetmek (parayi, oyunu : &ouml;rne&gbreve;in loto; sava&scedil;i,
  m&uuml;cadeleyi)</trd>
  <trd lng="uk">&c_p;&c_r;&c_o;&c_g;&c_r;&c_a;&c_v;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#22833;&#x843D; <pr>sh&#x12B;lu&#xF2;</pr></trd>,
    <trd>&#x4E1F;&#22833; <pr>di&#x16B;sh&#x12B;</pr></trd>,
    <trd>&#x8F38; <pr>sh&#x16B;</pr></trd>,
    <trd>&#x635F;&#22833; <pr>s&#x1D4;nsh&#x12B;</pr></trd>,
    <trd>&#36951; <pr>y&#xED;</pr></trd>,
    <trd>&#22833; <pr>sh&#x12B;</pr></trd>,
    <trd>&#x635F; <pr>s&#x1D4;n</pr></trd>,
    <trd>&#x4E27;&#22833; <pr>s&amacron;ngsh&#x12B;</pr></trd>
  </trdgrp>
</drv>
