<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>kreditor</rad>/o <fnt>Z</fnt>
</kap>
<drv mrk="kredit1.0">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Tiu, al kiu oni &scirc;uldas sumon:
      <ekz>
        ne helpas plendo nek ploro
        kontra&ubreve; <tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="debito.0o">debitoro</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Gl&auml;ubiger</trd>,
    <trd>Kreditor</trd>,
    <trd>Kreditgeber</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>creditor</trd>,
    <trd>truster</trd>,
    <trd>account payable (US)</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schuldeiser</trd>,
    <trd>crediteur</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_d;&c_i;&c_t;&c_o;&c_r;</trd>
</drv>
</art>
