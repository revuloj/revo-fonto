<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: plejst.xml,v 1.17 2023/10/20 16:38:17 revo Exp $">
<kap>
  <rad>plejstocen</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="plejst.0o">
  <kap><tld/>o</kap>
  <snc mrk="plejst.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Geologia <ref tip="super" cel="kvater.0a.GEOL">kvaternara</ref>
      <ref tip="lst" cel="epok.0o.periodo" lst="voko:geologiaj_epokoj" val="-1806000">epoko</ref>
      inter <ref cel="plioce.0o">plioceno</ref> kaj <ref cel="holocen.0o">holoceno</ref>, dum kiu
      evolui&gcirc;is la moderna
      <ref tip="prt" cel="hom.0o.ZOO">homo</ref>:
      <ekz>
        <tld/>o estas la unua epoko de la kvaternara periodo, &ccirc;efe ampleksanta la
        glacian a&gcirc;on (diluvo)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19793">Plejstoceno</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_je;&c_j;&c_s;&c_t;&c_a;&c_c;&c_e;&c_n;</trd>
  <trd lng="bg">&c_p;&c_l;&c_je;&c_j;&c_s;&c_t;&c_o;&c_c;&c_je;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>diluvium</trd>,
    <trd>pleistoc&eacute;n</trd>,
    <trd>v&ecaron;k ledov&yacute;ch dob</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Pleistoz&auml;n</trd>,
     <trd>Eiszeitalter</trd>
  </trdgrp>
  <trd lng="en">Pleistocene</trd>
  <trd lng="fa">&f_p;&a_l;&f_y;&a_s;&a_t;&a_w;&a_s;&a_n;</trd>
  <trd lng="fr">Pl&eacute;istoc&egrave;ne</trd>
  <trd lng="hu">pleisztoc&eacute;n</trd>
  <trdgrp lng="ja">
    <trd>&#x66F4;&#x65B0;&#x4E16;  <pr>&#x3053;&#x3046;&#x3057;&#x3093;&#x305B;&#x3044;</pr></trd>,
    <trd>&#x66F4;&#x65B0;&#x7D71;  <pr>&#x3053;&#x3046;&#x3057;&#x3093;&#x7D71;</pr></trd>
  </trdgrp>
  <trd lng="nl">pleistoceen</trd>
  <trd lng="pl">plejstocen</trd>
  <trdgrp lng="pt">
    <trd>Plistoceno</trd>,
    <trd>Pleistoceno</trd>
  </trdgrp>
  <trd lng="ro">pleistocen</trd>
  <trd lng="ru">&c_p;&c_l;&c_je;&c_j;&c_s;&c_t;&c_o;&c_c;&c_je;&c_n;</trd>
  <trd lng="sk">pleistoc&eacute;n</trd>
  <trd lng="uk">&#x43F;&#x43B;&c_je;&c_j;&c_s;&c_t;&#x43E;&c_c;&c_je;&#x43D;</trd>
</drv>
</art>
