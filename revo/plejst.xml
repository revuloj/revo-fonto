<vortaro>
<art mrk="$Id$">
<kap>
  <rad>plejstocen</rad>/o
</kap>
<drv mrk="plejst.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Formacio de la kvaternara epoko.
    </dif>
    <refgrp tip="vid">
      <ref cel="aluvi">aluvio</ref>,
      <ref cel="diluv.0o">diluvo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_je;&c_j;&c_s;&c_t;&c_a;&c_c;&c_e;&c_n;</trd>
  <trdgrp lng="de">
     <trd>Pleistoz&auml;n</trd>,
     <trd>Eiszeitalter</trd>
  </trdgrp>
  <trd lng="hu">pleisztoc&eacute;n</trd>
  <trd lng="nl">pleistoceen</trd>
  <trdgrp lng="pt">
    <trd>Plistoceno</trd>,
    <trd>Pleistoceno</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_l;&c_je;&c_j;&c_s;&c_t;&c_o;&c_c;&c_je;&c_n;</trd>
</drv>
</art>
