<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: miriam.xml,v 1.18 2024/01/24 09:24:56 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>miriametr</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="miriam.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Dek mil <ref tip="prt" cel="metr.0o.unuo">metroj</ref>:
      <ekz>
        historia indikilo: &leftquot;3 miriametrojn&rightquot; (30 kilometrojn) al la urbo
        <nom>Zerbst</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=178043">K&ouml;selitz</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        nek miria-, nek <tld/>o estas plu uzataj
        <fnt>
          <bib>PMEG</bib>
          <vrk><url
          ref="https://bertilow.com/pmeg/vortfarado/neoficialaj_afiksoj/internaciaj.html">Prefiksoj
          de mezurunuoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_r;&c_y;&c_ja;&c_m;&c_e;&c_t;&c_a;&c_r;</trd>
  <trd lng="ca">miri&agrave;metre</trd>
  <trd lng="cs">myriametr</trd>
  <trd lng="de">Myriameter</trd>
  <trdgrp lng="en">
    <trd>myriametre</trd>,
    <trd>myriameter</trd>
  </trdgrp>
  <trd lng="fr">myriam&egrave;tre</trd>
  <trd lng="hu">miriam&eacute;ter</trd>
  <trd lng="ja">&#x30DF;&#x30EA;&#x30A2;&#x30E1;&#x30FC;&#x30C8;&#x30EB;</trd>
  <trd lng="nl">myriameter</trd>
  <trd lng="ru">&c_m;&c_i;&c_r;&c_i;&c_a;&c_m;&c_je;&c_t;&c_r;</trd>
  <trd lng="sk">myriameter</trd>
  <trd lng="uk">&#x43C;&c_ib;&c_r;&c_ib;&c_a;&#x43C;&c_je;&c_t;&c_r;</trd>
</drv>
</art>
