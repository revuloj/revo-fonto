<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: sxtud.xml,v 1.2 2024/10/28 07:28:54 afido Exp $">
    <kap>
      <rad>&scirc;tud</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="sxtud.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="stift.0o">Stifto</ref> kun &scirc;ra&ubreve;bkanelo,
      t.e. senkapa &scirc;ra&ubreve;bo, &scirc;ra&ubreve;bostifto:
      <ekz>
        temas pri truhava <tld/>o, a&ubreve; ventoltubeto (korno), videbla en la suba bildo de
        M16, al la du amba&ubreve;flankaj kornoj oni povis fiksi aldonan &scirc;talan
        frunto&scirc;irmilon kontra&ubreve; deproksimaj pafoj
        <fnt>
          <bib>LOdE</bib>
          <vrk><url ref="https://esperanto-ondo.ru/Ondo/245-lode.htm"
              >pri &leftquot;G&uuml;nter Grass: Mia jarcento&rightquot;, &ccirc;ap. 1915</url></vrk>
          <lok>n-ro 245, 03/2015</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Gewindestift</trd>,
      <trd>Gewindestange</trd>,
      <trd>Stiftschraube</trd>,
      <trd>Stehbolzen</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>stud</trd>,
      <trd>boiler stay</trd>
    </trdgrp>
    <trd lng="fr">goujon</trd>
    <trdgrp lng="ru">
      <trd>&c_u;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_o;&c_ch;&c_n;&c_y;&c_j; &c_sh;&c_t;&c_i;&c_f;&c_t;</trd>,
      <trd>&c_sh;&c_p;&c_i;&c_l;&c_mol;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
<trdgrp lng="be">
  <trd>&c_sh;&c_t;&c_y;&c_f;&c_t;</trd>,
  <trd>&c_sh;&c_p;&c_ib;&c_l;&c_mol;&c_k;&c_a; <klr>(&c_z;&c_a;&c_m;&c_a;&c_c;&c_o;&c_w;&c_v;&c_a;&c_ju;&c_ch;&c_a;&c_ja;)</klr></trd>
</trdgrp>
</drv>

  </art>

