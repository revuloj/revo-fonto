<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: glicer.xml,v 1.20 2023/08/31 14:29:53 revo Exp $">
<kap>
  <rad>gliceri</rad>/o
</kap>
<drv mrk="glicer.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Glyceria</trd>):
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> estas sta&ubreve;da senhara herbo kaj havas stolonojn
        kaj grapolaron el 3-14-floraj senaristaj spiketoj, <tld/>oj estas holarktaj kaj dol&ccirc;akvejaj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=371674">Glyceria</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_n;&c_mol;&c_n;&c_ib;&c_k; <klr tip="amb">(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>,
    <trd>&c_m;&c_a;&c_n;&c_n;&c_a; <klr tip="amb">(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="ca">g&egrave;nere Glyceria</trd>
  <trdgrp lng="de">
     <trd>Schwaden</trd>,
     <trd>Mannagras</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Glyceria</trd>,
    <trd>mannagrass</trd>, <trd>sweetgrass</trd>
  </trdgrp>
  <trd lng="fa">&a_sh;&f_y;&a_r;&f_y;&a_n;&zwnj;&f_ch;&a_m;&a_n;</trd>
  <trd lng="fr">glyc&eacute;rie</trd>
  <trd lng="ja">&#x30C9;&#x30B8;&#x30E7;&#x30A6;&#x30C4;&#x30CA;&#x30AE;</trd>
  <trd lng="nl">liesgras</trd>
  <trd lng="pl">manna</trd>
  <trd lng="pt">glic&eacute;ria</trd>
  <trd lng="ru">&c_m;&c_a;&c_n;&c_n;&c_i;&c_k;</trd>
  <trd lng="uk">&#x43B;&c_je;&#x43F;&c_je;&c_sh;&#x43D;&#x44F;&#x43A;</trd>
</drv>
</art>
