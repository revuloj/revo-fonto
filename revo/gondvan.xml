        inter <tld/>o kaj la norda La&ubreve;razio etendi&gcirc;is la
        Tetismaro, kies restoj formas la hodia&ubreve;an Mediteraneon
        <fnt><bib>Viki</bib>,
        <lok>artikolo &leftquot;Gondvano&rightquot;, 2008-03-09</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">Gondwana</trd>
  <trd lng="pl">Gondwana</trd>
  <trdgrp lng="pt">
    <trd>Gondwana</trd>,
    <trd>Gonduana</trd>
  </trdgrp>
</drv>
</art>

