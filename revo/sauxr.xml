<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: sauxr.xml,v 1.1 2019/08/05 17:10:14 revo Exp $">
    <kap>
      <rad>sa&ubreve;r</rad>/o
    </kap>

<drv mrk="sauxr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PAL</uzo><uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Fosilia <ref tip="super" cel="reptil.0o">reptilio</ref>:
      <ekz>
        <nom>Gingko biloba</nom> <klr>[&#8230;]</klr> unue floris anta&ubreve; 350 milionoj da jaroj &dash; do, anta&ubreve;
        la veraj arboj, pli frue ol la <tld/>oj, e&ccirc; anta&ubreve; ol la insektoj, rampis surtere
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006878.html"
              >Dommastrumo: &Gcirc;ardenumi</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> nekonata sa&ubreve;ro tuj ricevis labornomon en la latina lingvo
        <fnt>
          <bib>Monato</bib>
          <aut>Hans B. Welling</aut>
          <vrk><url ref="https://www.monato.net/2014/011331.php"
              >Nesuspektindaj flutistoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kolekto da skeletoj kaj fosilioj de <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=440065"
              >Kastelo Friedenstein (Gotha)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="super">
       <ref cel="dinosa.0o">dinosa&ubreve;ro</ref>,
       <ref cel="ihxtio1.0o">i&hcirc;tiosa&ubreve;ro</ref>
    </refgrp>
    <trd lng="de">Saurier</trd>
  </snc>
</drv>

  </art>

