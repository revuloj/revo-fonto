<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ebon</rad>/o
</kap>
<drv mrk="ebon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Speco de bele nigra kaj tre malmola ligno:
      <ekz>
        eburon kaj <tld/>on ili vendadis al vi
        <fnt><bib>MT</bib>, <lok>&Jeh; 27:15</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">eben</trd>
  <trd lng="de">Ebenholz</trd>
  <trd lng="en">ebony</trd>
  <trd lng="es">&eacute;bano</trd>
  <trd lng="fr">&eacute;b&egrave;ne</trd>
  <trd lng="hu">&eacute;ben(fa)</trd>
  <trd lng="nl">ebbenhout</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_jo;&c_r;&c_n;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>,
    <trd>&c_e;&c_b;&c_je;&c_n;&c_o;&c_v;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="sv">ebenholts</trd>
</drv>

<drv mrk="ebon.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      El <tld/>o:
      <ekz>
        <tld/>a skatolo.
      </ekz>
    </dif>
    <trd lng="en">ebony</trd>
    <trd lng="ru">&c_e;&c_b;&c_je;&c_n;&c_o;&c_v;&c_y;&c_j;</trd>
  </snc>
  <trd lng="hu">&eacute;benfa</trd>
  <snc>
    <dif>
      <tld lit="E"/>kolora:
      <ekz>
        <tld/>aj haroj.
      </ekz>
    </dif>
    <trd lng="en">ebony</trd>
    <trd lng="ru">&c_ch;&c_jo;&c_r;&c_n;&c_y;&c_j; &c_k;&c_a;&c_k; &c_s;&c_m;&c_o;&c_l;&c_mol;</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>d'<ind>&eacute;b&egrave;ne</ind></trd>,
    <trd>en <ind>&eacute;b&egrave;ne</ind></trd>
  </trdgrp>
  <trd lng="hu">&eacute;ben</trd>
</drv>

<drv mrk="ebon.0ujo0arbo">
  <kap><tld/>ujo, <tld/>arbo</kap>
  <snc>
    <dif>
      Arbo produktanta <tld/>on (diospyros ebenus).
    </dif>
    <trd lng="en">ebony</trd>
  </snc>
  <trd lng="fr">&eacute;b&eacute;nier</trd>
  <trd lng="hu">&eacute;benfa</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_jo;&c_r;&c_n;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;<klr tip="ind"> (&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>,
    <trd>&c_e;&c_b;&c_je;&c_n;&c_o;&c_v;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;<klr tip="amb"> (&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="ebon.0arto">
  <kap><tld/>arto</kap>
  <snc>
    <dif>
      Arto konstrui meblojn a&ubreve; simila&jcirc;ojn la&ubreve; procedoj,
      kiuj malebligas, ke ties paneloj estu influataj de normala
      temperaturo.
    </dif>
  </snc>
  <trd lng="fr">&eacute;b&eacute;nisterie</trd>
  <trd lng="ru">&c_r;&c_je;&c_m;&c_je;&c_s;&c_l;&c_o; <ind>&c_k;&c_r;&c_a;&c_s;&c_n;&c_o;&c_d;&c_je;&c_r;&c_je;&c_v;&c_c;&c_a;</ind></trd>
</drv>

<drv mrk="ebon.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Metiisto kiu konstruas meblojn la&ubreve; <tld/>artaj procedoj.
    </dif>
  </snc>
  <trd lng="ca">ebenista</trd>
  <trd lng="es">ebanista</trd>
  <trd lng="fr">&eacute;b&eacute;niste</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_a;&c_s;&c_n;&c_o;&c_d;&c_je;&c_r;&c_je;&c_v;&c_je;&c_c;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_s;&c_n;&c_o;&c_d;&c_je;&c_r;&c_je;&c_v;&c_shch;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
<adm>
  ~arto kaj ~isto malaperis el PIV1. Ver&scirc;ajne tiuj terminoj
  estas pausa&jcirc;oj de la franca kaj &ccirc;iuokaze necesas kontroli
  ilian sencon kaj direkti al prefereblaj anstata&ubreve;a&jcirc;oj,
  se ekzistas. <aut>MB</aut>
</adm>
</art>
