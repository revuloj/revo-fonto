<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ebon.xml,v 1.30 2025/07/17 20:50:51 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ebon</rad>/o
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="ebon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Bele nigra, a&ubreve; nigrastria kaj tre malmola <ref tip="super" cel="lign.0o">ligno</ref>
      de arboj el la familio <nom>Diospyros</nom>:
      <ekz>
        eburon kaj <tld/>on ili vendadis al vi
        <fnt><bib>MT</bib>, <lok>&Jeh; 27:15</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;iaj haroj estis nigraj kiel <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Juda knabino</lok>
        </fnt>;
      </ekz>
      <ekz>
        granda lito, inkrustita per <tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mahago.0o.ligno">mahagono<sncref/></ref>,
      <ref cel="palisa.0o">palisandro</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_b;&c_e;&c_n;</trd>,
    <trd>&c_e;&c_b;&c_e;&c_n;&c_a;&c_v;&c_a;&c_je; <ind>&c_d;&c_r;&c_e;&c_v;&c_a;</ind></trd>,
    <trd>&c_ch;&c_o;&c_r;&c_n;&c_a;&c_je; <ind>&c_d;&c_r;&c_e;&c_v;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="bo">&t_ba;&t_tsheg;&t_sa;&t_naro;&t_tsheg;</trd>
  <trdgrp lng="ca">
    <trd>ban&uacute;s</trd>,
    <trd>eben</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>eben</trd>,
    <trd>tomel</trd>
  </trdgrp>
  <trd lng="de">Ebenholz</trd>
  <trd lng="en">ebony</trd>
  <trd lng="es">&eacute;bano</trd>
  <trd lng="fa">&a_A_madda;&a_b;&a_n;&a_w;&a_s;</trd>
  <trd lng="fr">&eacute;b&egrave;ne</trd>
  <trd lng="hu">&eacute;ben(fa)</trd>
  <trd lng="ja">&#40658;&#12383;&#12435;  <pr>&#12367;&#12429;&#12383;&#12435;</pr></trd>
  <trd lng="nl">ebbenhout</trd>
  <trd lng="pl">heban</trd>
  <trd lng="pt">&eacute;bano</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_jo;&c_r;&c_n;&c_o;&c_je;
    &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>,
    <trd>&c_e;&c_b;&c_je;&c_n;&c_o;&c_v;&c_o;&c_je;
    &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">eben</trd>
  <trd lng="sv">ebenholts</trd>
  <trdgrp lng="uk">
    <trd>&c_ch;&c_o;&c_r;&c_n;&c_je;</trd>,
    <trd>&c_je;&c_b;&c_je;&c_n;&c_o;&c_v;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o; (&c_m;&c_a;&c_t;&c_je;&c_r;&c_ib;&c_a;&c_l;)</trd>
  </trdgrp>
  <trd lng="zh">&#40671; <pr>y&imacron;</pr></trd>
</drv>

<drv mrk="ebon.0a">
  <kap><tld/>a</kap>
  <snc mrk="ebon.0a.ligna">
    <dif>
      El <tld/>o:
      <ekz>
        <klr>[li]</klr> mal&scirc;losis <tld/>an skatoleton
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">de <ind>ban&uacute;s</ind></trd>
    <trd lng="de">Ebenholz-</trd>
    <trd lng="en">ebony</trd>
    <trd lng="ru">&c_e;&c_b;&c_je;&c_n;&c_o;&c_v;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="ebon.0a.kolora">
     <dif>
       <tld lit="E"/>kolora:
       <ekz>
         <tld/>aj haroj.
       </ekz>
     </dif>
     <refgrp tip="vid">
      <ref cel="nigr.0a.KOL">nigra<sncref/></ref>,
    </refgrp>
     <trd lng="ca">de color <ind>ban&uacute;s</ind></trd>
     <trdgrp lng="de">
       <trd>ebenholzfarben</trd>,
       <trd>tiefdunkel</trd>
     </trdgrp>
     <trd lng="en">ebony</trd>
     <trd lng="ru">&c_ch;&c_jo;&c_r;&c_n;&c_y;&c_j; &c_k;&c_a;&c_k;
       &c_s;&c_m;&c_o;&c_l;&c_mol;</trd>
  </snc>
  <trd lng="be">&c_e;&c_b;&c_e;&c_n;&c_a;&c_v;&c_y;</trd>
  <trd lng="ca">de color <ind>ban&uacute;s</ind></trd>
  <trdgrp lng="cs">
    <trd>ebenov&eacute; barva</trd>,
    <trd>ebenov&yacute;</trd>
  </trdgrp>
  <trd lng="en">ebony</trd>
  <trd lng="fa">&a_A_madda;&a_b;&a_n;&a_w;&a_s;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>d'<ind>&eacute;b&egrave;ne</ind></trd>,
    <trd>en <ind>&eacute;b&egrave;ne</ind></trd>
  </trdgrp>
  <trd lng="he">&mem;&ayin;&ftsadi; &he;&#1464;&bet;&#1456;&nun;&#1462;&he;</trd>
  <trd lng="hu">&eacute;benfa</trd><trd lng="hu">&eacute;ben</trd>
  <trdgrp lng="ja">
    <trd>&#40658;&#12383;&#12435;&#12398;  <pr>&#12367;&#12429;&#12383;&#12435;&#12398;</pr></trd>,
    <trd>&#28422;&#40658;&#12398;  <pr>&#12375;&#12387;&#12371;&#12367;&#12398;</pr></trd>
  </trdgrp>
  <trd lng="pl">hebanowy</trd>
  <trd lng="ru">&c_ch;&c_jo;&c_r;&c_n;&c_y;&c_j; &c_k;&c_a;&c_k;
    &c_s;&c_m;&c_o;&c_l;&c_mol;</trd>
  <trd lng="sk">ebenov&yacute;</trd>
</drv>

<drv mrk="ebon.0ujo0arbo">
  <kap><tld/>ujo, <var><kap><tld/>arbo</kap></var></kap>
  <snc>
    <dif>
      <ref tip="super" cel="arb.0o.BOT">Arbo</ref> produktanta <tld/>on (<trd lng="la">Diospyros ebenum</trd> k.a.);
      en pli vasta senco la genro  <ref tip="super" cel="persimon.0ujo">persimonujo</ref>.
    </dif>
    <trd lng="en">ebony</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_b;&c_e;&c_n;</trd>,
    <trd>&c_e;&c_b;&c_e;&c_n;&c_a;&c_v;&c_a;&c_je; <ind>&c_d;&c_r;&c_e;&c_v;&c_a;</ind></trd>,
    <trd>&c_ch;&c_o;&c_r;&c_n;&c_a;&c_je; <ind>&c_d;&c_r;&c_e;&c_v;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="ca">ban&uacute;s</trd>
  <trdgrp lng="cs">
    <trd>eben</trd>,
    <trd>ebenovn&iacute;k</trd>,
    <trd>ebenov&yacute; strom</trd>
  </trdgrp>
  <trd lng="de">Ebenholzbaum</trd>
  <trd lng="fa">&a_d;&a_r;&a_kh;&a_t;
  <ind>&a_A_madda;&a_b;&a_n;&a_w;&a_s;</ind></trd>
  <trd lng="fr">&eacute;b&eacute;nier</trd>
  <trd lng="he">&ayin;&ftsadi; &he;&bet;&nun;&he;</trd>
  <trd lng="hu">&eacute;benfa</trd>
  <trd lng="ja">&#12467;&#12463;&#12479;&#12531;</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_jo;&c_r;&c_n;&c_o;&c_je;
    &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;<klr tip="ind">
    (&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>,
    <trd>&c_e;&c_b;&c_je;&c_n;&c_o;&c_v;&c_o;&c_je;
    &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;<klr tip="amb">
    (&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="sk">ebenovn&iacute;k</trd>
  <trd lng="uk">&c_je;&c_b;&c_je;&c_n;&c_o;&c_v;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  <trd lng="zh">&#20044;&#26408; <pr>w&umacron;m&ugrave;</pr></trd>
</drv>

<drv mrk="ebon.0arto">
  <kap><tld/>arto</kap>
  <snc>
    <dif>
      Arto konstrui meblojn a&ubreve; simila&jcirc;ojn la&ubreve;
      procedoj,
      kiuj malebligas, ke ties paneloj estu influataj de normala
      temperaturo.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_m;&c_ja;&c_s;&c_t;&c_v;&c_o; <ind>&c_ch;&c_y;&c_r;&c_v;&c_a;&c_n;&c_a;&c_d;&c_r;&c_e;&c_w;&c_sh;&c_ch;&c_y;&c_k;&c_a;</ind></trd>,
    <trd>&c_v;&c_y;&c_r;&c_a;&c_b; &c_r;&c_e;&c_ch;&c_a;&c_w; &c_z; &c_e;&c_b;&c_e;&c_n;&c_u;</trd>
  </trdgrp>
  <trd lng="ca">ebenisteria</trd>
  <trd lng="cs">um&ecaron;leck&eacute; zpracov&aacute;n&iacute; ebenu</trd>
  <trd lng="de">Ebenholzkunst</trd>
  <trd lng="fr">&eacute;b&eacute;nisterie</trd>
  <trd lng="ja">&#40658;&#12383;&#12435;&#23478;&#20855;&#35069;&#36896;&#25216;&#34899;
  <pr>&#12367;&#12429;&#12383;&#12435;&#12363;&#12368;&#12379;&#12356;&#12382;&#12358;&#12366;&#12376;&#12421;&#12388;</pr></trd>
  <trd lng="ru">&c_r;&c_je;&c_m;&c_je;&c_s;&c_l;&c_o;
  <ind>&c_k;&c_r;&c_a;&c_s;&c_n;&c_o;&c_d;&c_je;&c_r;&c_je;&c_v;&c_c;&c_a;</ind></trd>
  <trd lng="sk">umeleck&eacute; spracovanie ebenu</trd>
</drv>

<drv mrk="ebon.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Metiisto kiu konstruas meblojn la&ubreve; <tld/>artaj procedoj.
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_y;&c_r;&c_v;&c_a;&c_n;&c_a;&c_d;&c_r;&c_e;&c_w;&c_sh;&c_ch;&c_y;&c_k;</trd>
  <trd lng="ca">ebenista</trd>
  <trd lng="cs">zpracovatel ebenu</trd>
  <trd lng="de">Ebenist</trd>
  <trd lng="es">ebanista</trd>
  <trd lng="fr">&eacute;b&eacute;niste</trd>
  <trdgrp lng="ja">
    <trd>&#40658;&#12383;&#12435;&#23478;&#20855;&#24107;  <pr>&#12367;&#12429;&#12383;&#12435;&#12363;&#12368;&#12375;</pr></trd>,
    <trd>&#40658;&#12383;&#12435;&#32887;&#20154;  <pr>&#12367;&#12429;&#12383;&#12435;&#12375;&#12423;&#12367;&#12395;&#12435;</pr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_a;&c_s;&c_n;&c_o;&c_d;&c_je;&c_r;&c_je;&c_v;&c_je;&c_c;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_s;&c_n;&c_o;&c_d;&c_je;&c_r;&c_je;&c_v;&c_shch;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">spracovate&lcaron; ebenu</trd>
</drv>
<adm>
  ~arto kaj ~isto malaperis el PIV1. Ver&scirc;ajne tiuj terminoj
  estas pausa&jcirc;oj de la franca kaj &ccirc;iuokaze necesas kontroli
  ilian sencon kaj direkti al prefereblaj anstata&ubreve;a&jcirc;oj,
  se ekzistas. <aut>MB</aut>
</adm>
</art>
