<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: anelid.xml,v 1.19 2023/10/28 08:33:06 revo Exp $">
<kap>
  <rad>anelid</rad>/oj <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="anelid.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="anelid.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="verm.ringo0oj.ZOO">Ringovermoj</ref>:
      <ekz>
        <tld/>oj a&ubreve; ringvermoj estas filumo de vermoj en kiu la surfaco de la korpo
        montras pli-malpli distingitajn dividojn <klr>[de]</klr> ringoj a&ubreve; segmentoj,
        &gcirc;enerale havantaj alfiksa&jcirc;ojn por movado kaj brankojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=28534">Anelidoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
       <ref cel="lumbri.0o">lumbriko</ref>,
       <ref cel="hirud.0o">hirudo</ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#x73AF;&#x8282;&#x52A8;&#x7269;&#x95E8; <pr>hu&#xE1;nji&#xE9;d&#xF2;ngw&#xF9;m&#xE9;n</pr></trd>,
    <trd>&#x74B0;&#x7BC0;&#x52D5;&#x7269;&#x9580; <pr>hu&#xE1;nji&#xE9;d&#xF2;ngw&#xF9;m&#xE9;n</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_k;&c_o;&c_l;&c_c;&c_a;&c_v;&c_y;&c_ja; &c_ch;&c_a;&c_r;&c_v;&c_ja;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_p;&c_r;&c_je;&c_sh;&c_l;&c_je;&c_n;&c_je;&c_s;&c_t;&c_i; &c_ch;&c_je;&c_r;&c_v;&c_je;&c_i;</trd>
  <trdgrp lng="de">
    <trd>Ringelw&uuml;rmer</trd>,
    <trd>Gliederw&uuml;rmer</trd>
  </trdgrp>
  <trd lng="es">an&eacute;lidos</trd>
  <trd lng="fr">ann&eacute;lides</trd>
  <trd lng="hu">gy&udblac;r&udblac;sf&eacute;rgek</trd>
  <trd lng="ja">&#x74B0;&#x5F62;&#x52D5;&#x7269;  <pr>&#x304B;&#x3093;&#x3051;&#x3044;&#x3069;&#x3046;&#x3076;&#x3064;</pr></trd>
  <trd lng="nl">ringwormen</trd>
  <trd lng="pl">pier&sacute;cienice</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_mol;&c_ch;&c_a;&c_t;&c_y;&c_je; &c_ch;&c_je;&c_r;&c_v;&c_i;</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_ib;&#x43B;&#x44C;&c_ch;&c_a;&c_s;&c_t;&c_ib; &c_ch;&c_je;&c_r;&c_v;&c_i;</trd>,
    <trd>&#x43A;&c_ib;&#x43B;&#x44C;&c_ch;&c_a;&#x43A;&c_i;</trd>,
    <trd>&c_a;&#x43D;&c_je;&#x43B;&c_ib;&c_d;&c_i;</trd>
  </trdgrp>
</drv>
</art>
