<kap>
  <ofc>*</ofc>
  <rad>rab</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="rab.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rab.0i.JUR">
    <dif>
      Perforte <ref tip="super" cel="pren.for0i">forpreni</ref> tion, 
      kio apartenas al alia:
      <ekz>
        <klr>[ili]</klr> <tld/>is &ccirc;ion, kio estis en la domoj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 34:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        ilian tutan poseda&jcirc;on kaj havon ili <tld/>is
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 31:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        la militistoj <tld/>is &ccirc;iu por si
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 31:53</lok>
        </fnt>;
      </ekz>
      <ekz>
        li redonu la <tld/>ita&jcirc;on, kiun li <tld/>is
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 6:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        la popolo eliris, kaj <ind>dis<tld/>i</ind>s la tendaron
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 7:16</lok>
        </fnt>;
        <trdgrp lng="ca">
          <trd>saquejar</trd>,
          <trd>desfalcar</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        vi volas <tld/>i de mi la lastan konsolon &ccirc;e la morto
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        la morto <tld/>is al ni tre multe el niaj fervoraj kunbatalantoj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista en Boulogne sur Mer en la 5a de a&ubreve;gusto
          1905</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mal&gcirc;ojo al li <tld/>as la feli&ccirc;on
        <fnt>
          <bib>Ifigenio</bib>
          <lok>Ifigenio en Ta&ubreve;rido</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu <tld/>i eliras, ofte nuda revenas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kapt.0i">kapti</ref>,
      <ref cel="sxir">for&scirc;iri</ref>,
      <ref cel="sxtel.0i">&scirc;teli</ref>
    </refgrp>
    <trd lng="de">rauben</trd>
    <trd lng="th">&#xE1B;&#xE25;&#xE49;&#xE19;</trd>
  </snc>
  <snc mrk="rab.0i.prirabi">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="rab.pri0i">Prirabi</ref>, senhavigi:
      <ekz>
        mi hontas vin <tld/>i, mia an&gcirc;elo, mi scias, ke vi havas apena&ubreve; sufi&ccirc;e por vi
        <fnt>
          <bib>IKr</bib>
          <lok>Patro</lok>
        </fnt>;
      </ekz>   
      <ekz>
        bandoj da ili vagas de loko al loko, por <tld/>i kaj mortigi &ccirc;iujn, kiujn ili
        renkontas
        <fnt>
          <bib>Rob</bib>
          <lok>Parto V</lok>
        </fnt>.
      </ekz>   
    </dif>
    <trd lng="de">berauben</trd>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bo">&t_ra;&t_kata;&t_shabchu;&t_tsheg;&t_ma;&t_tsheg;&t_ba;&t_ra;&t_kata;&t_shabchu;&t_sa;&t_tsheg;</trd>
  <trdgrp lng="ca">
    <trd>robar</trd>,
    <trd>furtar</trd>,
    <trd>prendre</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>drancovat</trd>,
    <trd>loupit</trd>,
    <trd>olupovat</trd>,
    <trd>rabovat</trd>
  </trdgrp>
  <trd lng="en">rob</trd>
  <trdgrp lng="es">
    <trd>atracar</trd>,
    <trd>robar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ravir</trd>,
    <trd>voler<klr> (d&eacute;rober)</klr></trd>
  </trdgrp>
  <trd lng="hu">rabol</trd>
  <trdgrp lng="ja">
    <trd>&#x596A;&#x3046;  [<ind>&#x3046;&#x3070;&#x3046;</ind>]</trd>,
    <trd>&#x596A;&#x3044;&#x53D6;&#x308B;  [<ind>&#x3046;&#x3070;&#x3044;&#x3068;&#x308B;</ind>]</trd>,
    <trd>&#x5F37;&#x596A;&#x3059;&#x308B;  [<ind>&#x3054;&#x3046;&#x3060;&#x3064;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x53D6;&#x308A;&#x4E0A;&#x3052;&#x308B;  [<ind>&#x3068;&#x308A;&#x3042;&#x3052;&#x308B;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>roven</trd>,
    <trd>plunderen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rabowa&cacute;</trd>,
    <trd>grabi&cacute;</trd>,
    <trd>grasowa&cacute;</trd>,
    <trd>&lstroke;upi&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>roubar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_a;&c_b;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_g;&c_r;&c_a;&c_b;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>l&uacute;pi&tcaron;</trd>,
    <trd>plieni&tcaron;</trd>,
    <trd>rabova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>r&ouml;va</trd>,
    <trd>plundra</trd>,
    <trd>r&aring;na</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_r;&c_a;&c_b;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&#x43E;&c_z;&#x43A;&c_r;&c_a;&c_d;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&#x43A;&c_r;&c_a;&c_d;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_b;&c_i;&c_r;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_b;&c_i;&c_r;&c_a;&c_t;&c_i; (&c_s;&c_i;&#x43B;&#x43E;&#x44E;)</trd>,
    <trd>&c_z;&c_a;&c_j;&#x43C;&c_a;&c_t;&c_i;&c_s;&#x44F; &c_r;&#x43E;&c_z;&c_b;&#x43E;&c_jeu;&#x43C;</trd>
  </trdgrp>
</drv>

<drv mrk="rab.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="rab.0ajxo.JUR">
    <dif>
      Io akirita per <tld/>ado:
      <ekz>
        vespere li dividos <tld/>a&jcirc;on
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 49:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        la brutojn ni prenis al ni, kiel <ref cel="akir.milit0o.ajxo">militakira&jcirc;o</ref>n, 
        kaj la <tld/>a&jcirc;on el la urboj,
        kiujn ni militakiris
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 2:35</lok>
        </fnt>;
      </ekz>
      <ekz>
        korsaro alvenis por forpreni lian <tld/>a&jcirc;on
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili foriris kun <tld/>a&jcirc;oj, lasante la garantiulojn
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Turkoj prenis min kiel sian <tld/>a&jcirc;on
        <fnt>
          <bib>Rob</bib>
          <lok>Parto I</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la varoj ku&scirc;as en la magazenoj, kiel publika <tld/>a&jcirc;o, el kiu &ccirc;iu forportas
        tiom multe, kiom li volas
        <fnt>
          <bib>VaK</bib>
          <lok>Kvara &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akir.0o">akiro</ref>,
      <ref cel="kapt.0ajxo">kapta&jcirc;o</ref>,
      <ref cel="pred.0o">predo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_r;&c_a;&c_b;&c_a;&c_v;&c_a;&c_n;&c_a;&c_je;</trd>
  <trd lng="ca">furt</trd>
  <trdgrp lng="de">
    <trd>Raubgut</trd>,
    <trd>Geraubtes</trd>
  </trdgrp>
  <trd lng="es">bot&iacute;n</trd>
  <trd lng="fr">butin</trd>
  <trd lng="hu">rablott holmi</trd>
  <trdgrp lng="ja">
    <trd>&#x5F37;&#x596A;&#x54C1;  [<ind>&#x3054;&#x3046;&#x3060;&#x3064;&#x3057;&#x306A;</ind>]</trd>,
    <trd>&#x7565;&#x596A;&#x54C1;  [<ind>&#x308A;&#x3083;&#x304F;&#x3060;&#x3064;&#x3072;&#x3093;</ind>]</trd>,
    <trd>&#x7372;&#x7269;  [<ind>&#x3048;&#x3082;&#x306E;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">buit</trd>
  <trdgrp lng="pl">
    <trd>&lstroke;up</trd>,
    <trd>zdobycz</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>presa</trd>,
    <trd>pilhagem</trd>,
    <trd>rapina</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_g;&c_r;&c_a;&c_b;&c_l;&c_je;&c_n;&c_n;&c_o;&c_je;</trd>
  <trd lng="sv">byte</trd>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_a;&c_g;&c_r;&c_a;&c_b;&#x43E;&c_v;&c_a;&#x43D;&c_je; (&c_u; &c_z;&#x43D;&c_a;&c_ch;. &c_ib;&#x43C;.)</trd>,
    <trd>&#x43D;&c_a;&c_g;&c_r;&c_a;&c_b;&#x43E;&c_v;&c_a;&#x43D;&c_je; &#x43C;&c_a;&c_j;&#x43D;&#x43E;</trd>,
    <trd>&#x43D;&c_a;&c_g;&c_r;&c_a;&c_b;&#x43E;&c_v;&c_a;&#x43D;&c_ib; &c_r;&c_je;&c_ch;&c_ib;</trd>
  </trdgrp>
</drv>

<drv mrk="rab.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Loko lo&gcirc;ata de <tld/>istoj:
      <ekz>
        &ccirc;iuj mallumaj lokoj de la tero estas plenaj de <tld/>ejoj
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 74:20</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>lloriguera</trd>,
    <trd>lladronera</trd>,
    <trd>amagatall</trd>
  </trdgrp>
  <trd lng="cs">pele&scaron; lupi&ccaron;&uring;</trd>
  <trd lng="de">R&auml;uberh&ouml;hle</trd>
  <trdgrp lng="es">
    <trd>ladronera</trd>,
    <trd>guarida</trd>
  </trdgrp>
  <trd lng="fr">repaire<klr> (de brigands)</klr></trd>
  <trd lng="hu">rabl&oacute;barlang</trd>
  <trd lng="ja">&#x76D7;&#x8CCA;&#x306E;&#x5DE3;&#x7A9F;  [<ind>&#x3068;&#x3046;&#x305E;&#x304F;&#x306E;&#x3059;&#x3044;&#x308F;&#x3084;</ind>]</trd>
  <trd lng="nl">rovershol</trd>
  <trd lng="pl">&zdot;erowisko</trd>
