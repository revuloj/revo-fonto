<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rab.xml,v 1.33 2021/05/29 14:33:09 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>rab</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="rab.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rab.0i.JUR">
    <dif>
      Perforte forpreni tion, kio apartenas al alia:
      <ekz>
        <tld/>i brutojn sur la kamparo;
      </ekz>
      <ekz>
        <tld/>i monon al, de, for de iu;
      </ekz>
      <ekz>
        la morto <tld/>is al li la infanon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la malamikaj bandoj <ind>dis<tld/>i</ind>s &ccirc;ie en la tuta
        lando;
        <trdgrp lng="ca">
          <trd>saquejar</trd>,
          <trd>desfalcar</trd>
        </trdgrp>
      </ekz>
      <ekz>
        dis<tld/>i la &scirc;tatajn financojn;
      </ekz>
      <ekz>
        for<tld/>i ies edzinon;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mal&gcirc;ojo <tld/>as al li la feli&ccirc;on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        vi volas <tld/>i de mi la lastan konsolon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la ludo de via imago <tld/>is al vi la klaran pensadon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kapt.0i">kapti</ref>,
      <ref cel="sxir">for&scirc;iri</ref>,
      <ref cel="sxtel.0i">&scirc;teli</ref>.
    </refgrp>
    <trd lng="th">&#xE1B;&#xE25;&#xE49;&#xE19;</trd>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bo">&#xF62;&#xF90;&#xF74;&#xF0B;&#xF58;&#xF0B;&#xF56;&#xF62;&#xF90;&#xF74;&#xF66;&#xF0B;</trd>
  <trdgrp lng="ca">
    <trd>robar</trd>,
    <trd>furtar</trd>,
    <trd>prendre</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>drancovat</trd>,
    <trd>loupit</trd>,
    <trd>olupovat</trd>,
    <trd>rabovat</trd>
  </trdgrp>
  <trd lng="de">rauben</trd>
  <trd lng="en">rob</trd>
  <trdgrp lng="es">
    <trd>atracar</trd>,
    <trd>robar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ravir</trd>,
    <trd>voler<klr> (d&eacute;rober)</klr></trd>
  </trdgrp>
  <trd lng="hu">rabol</trd>
  <trdgrp lng="nl">
    <trd>roven</trd>,
    <trd>plunderen</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>roubar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_a;&c_b;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_g;&c_r;&c_a;&c_b;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_h;&c_i;&c_shch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_h;&c_i;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>l&uacute;pi&tcaron;</trd>,
    <trd>plieni&tcaron;</trd>,
    <trd>rabova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>r&ouml;va</trd>,
    <trd>plundra</trd>,
    <trd>r&aring;na</trd>
  </trdgrp>
</drv>

<drv mrk="rab.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="rab.0ajxo.JUR">
    <dif>
      Io akirita per <tld/>ado.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_r;&c_a;&c_b;&c_a;&c_v;&c_a;&c_n;&c_a;&c_je;</trd>
  <trd lng="ca">furt</trd>
  <trdgrp lng="de">
    <trd>Raub</trd>,
    <trd>Geraubtes</trd>
  </trdgrp>
  <trd lng="es">bot&iacute;n</trd>
  <trd lng="fr">butin</trd>
  <trd lng="hu">rablott holmi</trd>
  <trd lng="nl">buit</trd>
  <trdgrp lng="pt">
    <trd>presa</trd>,
    <trd>pilhagem</trd>,
    <trd>rapina</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_g;&c_r;&c_a;&c_b;&c_l;&c_je;&c_n;&c_n;&c_o;&c_je;</trd>
  <trd lng="sv">byte</trd>
</drv>

<drv mrk="rab.0ejo">
  <kap><tld/>ejo<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Loko lo&gcirc;ata de <tld/>istoj.
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>lloriguera</trd>,
    <trd>lladronera</trd>,
    <trd>amagatall</trd>
  </trdgrp>
  <trd lng="cs">pele&scaron; lupi&ccaron;&uring;</trd>
  <trd lng="de">R&auml;uberh&ouml;hle</trd>
  <trdgrp lng="es">
    <trd>ladronera</trd>,
    <trd>guarida</trd>
  </trdgrp>
  <trd lng="fr">repaire<klr> (de brigands)</klr></trd>
  <trd lng="hu">rabl&oacute;barlang</trd>
  <trd lng="nl">rovershol</trd>
  <trd lng="pt">covil</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_b;&c_o;&c_j;&c_n;&c_i;&c_ch;&c_i;&c_j;
    &c_p;&c_r;&c_i;&c_t;&c_o;&c_n;</trd>,
    <trd>&c_l;&c_o;&c_g;&c_o;&c_v;&c_o;
    &c_r;&c_a;&c_z;&c_b;&c_o;&c_j;&c_n;&c_i;&c_k;&c_o;&c_v;</trd>
  </trdgrp>
  <trd lng="sk">pelech lupi&ccaron;ov</trd>
  <trd lng="sv">r&ouml;varh&aring;la</trd>
</drv>

<drv mrk="rab.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="rab.0isto.JUR">
    <dif>
      Homo, kiu kutime <tld/>as kaj vivas el <tld/>ado:
      <ekz>
        malri&ccirc;ulo <tld/>iston ne timas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bandit.0o">bandito</ref>,
      <ref cel="pirat.0o">pirato</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_b;&c_a;&c_w;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_b;&c_je;&c_zh;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="bo">&#xF47;&#xF42;&#xF0B;&#xF54;&#xF0B;</trd>
  <trd lng="ca">lladre</trd>
  <trdgrp lng="cs">
    <trd>loupe&zcaron;n&iacute;k</trd>,
    <trd>lupi&ccaron;</trd>,
    <trd>raub&iacute;&rcaron;</trd>
  </trdgrp>
  <trd lng="de">R&auml;uber</trd>
  <trdgrp lng="es">
    <trd>atracador</trd>,
    <trd>ladr&oacute;n</trd>,
    <trd>ratero</trd>
  </trdgrp>
  <trd lng="fr">brigand</trd>
  <trd lng="hu">rabl&oacute;</trd>
  <trd lng="nl">rover</trd>
  <trdgrp lng="pt">
    <trd>salteador</trd>,
    <trd>bandido</trd>,
    <trd>rapinante</trd>,
    <trd>ladr&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_a;&c_b;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_b;&c_o;&c_j;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>l&uacute;pe&zcaron;n&iacute;k</trd>,
    <trd>zbojn&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>r&ouml;vare</trd>,
    <trd>r&aring;nare</trd>
  </trdgrp>
</drv>

<drv mrk="rab.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rab.pri0i.JUR">
    <dif>
      <tld lit="R"/>e senposedigi iun je io:
      <ekz>
        pri<tld/>i voja&gcirc;anton, karavanon, urbon;
      </ekz>
      <ekz>
        tute dezertigita kaj pri<tld/>ita lando<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_r;&c_a;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">robar</trd>
  <trd lng="cs">vyloupit</trd>
  <trd lng="de">berauben</trd>
  <trdgrp lng="es">
    <trd>robar</trd>,
    <trd>hurtar</trd>,
    <trd>saltear<klr> (robar)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;trousser</trd>,
    <trd>piller</trd>
  </trdgrp>
  <trd lng="hu">kirabol</trd>
  <trd lng="nl">beroven</trd>
  <trdgrp lng="pt">
    <trd>saltear</trd>,
    <trd>pilhar</trd>,
    <trd>espoliar</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_g;&c_r;&c_a;&c_b;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>ol&uacute;pi&tcaron;</trd>,
    <trd>orabova&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">r&aring;na</trd>
</drv>

<drv mrk="rab.tempo0a">
  <kap>tempo<tld/>a</kap>
  <snc mrk="rab.tempo0a.KOMUNE">
    <dif>
      Kiu postulas multan tempon:
      <ekz>
        redaktado de bulteno, kaj de &ccirc;ia ajn revueto
        estas interesa kaj kreiva laboro,
        tamen tempo<tld/>a
        <fnt>
          <aut>Ma&ubreve;ro</aut>,
          <vrk>El la redakcio</vrk>,
          <lok><url ref="http://lsg-esperanto.org/143/forumo143.pdf">
          LSG-Forumo, 2009:2-3 (143)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">okr&aacute;daj&iacute;c&iacute; o &ccaron;as
  <klr>(adj.)</klr></trd>
  <trdgrp lng="es">
    <trd>que ocupa mucho tiempo</trd>,
    <trd>interminable</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>qui prend du temps</trd>,
    <trd>sans fin<klr> (tr&egrave;s long)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>uberaj&uacute;ci &ccaron;as</trd>,
    <trd>zd&lacute;hav&yacute;</trd>
  </trdgrp>
</drv>
</art>
