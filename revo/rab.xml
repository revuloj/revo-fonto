<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rab</rad>/i
</kap>
<drv mrk="rab.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Perforte forpreni tion, kio apartenas al alia:
      <ekz>
        <tld/>i brutojn sur la kamparo;
      </ekz>
      <ekz>
        <tld/>i monon al, de, for de iu;
      </ekz>
      <ekz>
        la morto <tld/>is al li la infanon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la malamikaj bandoj dis<tld/>is &ccirc;ie en la tuta lando;
      </ekz>
      <ekz>
        dis<tld/>i la &scirc;tatajn financojn;
      </ekz>
      <ekz>
        for<tld/>i ies edzinon;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mal&gcirc;ojo <tld/>as al li la feli&ccirc;on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        vi volas <tld/>i de mi la lastan konsolon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la ludo de via imago <tld/>is al vi la klaran pensadon.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kapt.0i">kapti</ref>,
      <ref cel="sxir">for&scirc;iri</ref>,
      <ref cel="sxtel.0i">&scirc;teli</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">loupit</trd>
  <trd lng="de">rauben</trd>
  <trd lng="en">rob</trd>
  <trd lng="hu">rabol</trd>
  <trdgrp lng="nl">
    <trd>roven</trd>,
    <trd>plunderen</trd>
  </trdgrp>
  <trd lng="fr">voler</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_a;&c_b;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_g;&c_r;&c_a;&c_b;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_h;&c_i;&c_shch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_h;&c_i;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>r&ouml;va</trd>,
    <trd>plundra</trd>,
    <trd>r&aring;na</trd>
  </trdgrp>
</drv>

<drv mrk="rab.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Io akirita per <tld/>ado.
    </dif>
  </snc>
  <trd
lng="be">&c_n;&c_a;&c_r;&c_a;&c_b;&c_a;&c_v;&c_a;&c_n;&c_a;&c_je;</trd>
  <trdgrp lng="de">
    <trd>Raub</trd>,
    <trd>Geraubtes</trd>
  </trdgrp>
  <trd lng="hu">rablott holmi</trd>
  <trd lng="nl">buit</trd>
  <trd lng="fr">butin</trd>
  <trd
lng="ru">&c_n;&c_a;&c_g;&c_r;&c_a;&c_b;&c_l;&c_je;&c_n;&c_n;&c_o;&c_je;</trd>
  <trd lng="sv">byte</trd>
</drv>

<drv mrk="rab.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Homo, kiu kutime <tld/>as kaj vivas el <tld/>ado:
      <ekz>malri&ccirc;ulo
        <tld/>iston ne timas
<fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="bandit.0o">bandito</ref>,
      <ref cel="pirat.0o">pirato</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_b;&c_a;&c_w;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_b;&c_je;&c_zh;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="de">R&auml;uber</trd>
  <trd lng="fr">voleur</trd>
  <trd lng="hu">rabl&oacute;</trd>
  <trd lng="nl">rover</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_a;&c_b;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_b;&c_o;&c_j;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>r&ouml;vare</trd>,
    <trd>r&aring;nare</trd>
  </trdgrp>
</drv>

<drv mrk="rab.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="R"/>e senposedigi iun je io:
      <ekz>
        pri<tld/>i voja&gcirc;anton, karavanon, urbon;
      </ekz>
      <ekz>
        tute dezertigita kaj pri<tld/>ita lando<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_r;&c_a;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">berauben</trd>
  <trd lng="hu">kirabol</trd>
  <trd lng="nl">beroven</trd>
  <trd lng="fr">d&eacute;trousser</trd>
  <trd lng="ru">&c_o;&c_g;&c_r;&c_a;&c_b;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sv">r&aring;na</trd>
</drv>

<drv mrk="rab.0ejo">
  <kap><tld/>ejo<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Loko lo&gcirc;ata de <tld/>istoj.
    </dif>
  </snc>
  <trd lng="de">R&auml;uberh&ouml;hle</trd>
  <trd lng="fr">repaire (de brigands)</trd>
  <trd lng="hu">rabl&oacute;barlang</trd>
  <trd lng="nl">rovershol</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_b;&c_o;&c_j;&c_n;&c_i;&c_ch;&c_i;&c_j;
&c_p;&c_r;&c_i;&c_t;&c_o;&c_n;</trd>,
    <trd>&c_l;&c_o;&c_g;&c_o;&c_v;&c_o;
&c_r;&c_a;&c_z;&c_b;&c_o;&c_j;&c_n;&c_i;&c_k;&c_o;&c_v;</trd>
  </trdgrp>
  <trd lng="sv">r&ouml;varh&aring;la</trd>
</drv>
</art>
