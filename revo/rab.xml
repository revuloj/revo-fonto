</drv>

<drv mrk="rab.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="rab.0isto.JUR">
    <dif>
      Homo, kiu kutime <tld/>as kaj vivas el <tld/>ado:
      <ekz>
        li transdonis ilin en la manojn de <tld/>istoj, kiuj pri<tld/>is ilin
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 2:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i embuskas kiel <tld/>isto, kaj kolektas &ccirc;irka&ubreve; si perfidulojn
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 23:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        malri&ccirc;ulo <tld/>iston ne timas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bandit.0o">bandito</ref>,
      <ref cel="pirat.0o">pirato</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_b;&c_a;&c_w;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_b;&c_je;&c_zh;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="bo">&t_ja;&t_ga;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trd lng="ca">lladre</trd>
  <trdgrp lng="cs">
    <trd>loupe&zcaron;n&iacute;k</trd>,
    <trd>lupi&ccaron;</trd>,
    <trd>raub&iacute;&rcaron;</trd>
  </trdgrp>
  <trd lng="de">R&auml;uber</trd>
  <trdgrp lng="es">
    <trd>atracador</trd>,
    <trd>ladr&oacute;n</trd>,
    <trd>ratero</trd>
  </trdgrp>
  <trd lng="fr">brigand</trd>
  <trd lng="hu">rabl&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x5F37;&#x76D7;  [<ind>&#x3054;&#x3046;&#x3068;&#x3046;</ind>]</trd>,
    <trd>&#x76D7;&#x8CCA;  [<ind>&#x3068;&#x3046;&#x305E;&#x304F;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">rover</trd>
  <trdgrp lng="pl">
    <trd>zb&oacute;j</trd>,
    <trd>grasant</trd>,
    <trd>grabie&zdot;ca</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>salteador</trd>,
    <trd>bandido</trd>,
    <trd>rapinante</trd>,
    <trd>ladr&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_a;&c_b;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_b;&c_o;&c_j;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>l&uacute;pe&zcaron;n&iacute;k</trd>,
    <trd>zbojn&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>r&ouml;vare</trd>,
    <trd>r&aring;nare</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_r;&c_a;&c_b;&c_ib;&c_zh;&#x43D;&c_i;&#x43A;</trd>,
    <trd>&c_r;&#x43E;&c_z;&c_b;&c_ib;&c_j;&#x43D;&c_i;&#x43A;</trd>
  </trdgrp>
</drv>

<drv mrk="rab.dis0i">
  <kap>dis<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rab.dis0i.dividi">
    <dif>
      <tld lit="R"/>i plurope por dividi inter si <tld/>a&jcirc;on:
      <ekz>
        Kaj la Izraelidoj revenis de la kurado post la Fili&scirc;toj kaj
        dis<tld/>is ilian tendaron
        <fnt><bib>MT</bib>, <lok>&Sa1; 17:53</lok></fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        dis<tld/>i la &scirc;tatajn financojn <fnt><bib>PV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="rab.dis0i.domagxi">
    <dif>
      Detrui, malplenigi, dama&gcirc;i iun lokon (urbon, lo&gcirc;ejon ktp) dum
      <tld/>ado:
      <ekz>
        Tre interesa kaj neevitebla vizito al la Re&gcirc;a Observatorio de
        <nom>San Fernando</nom>, unu el la malmultaj historiaj konstrua&jcirc;oj, kiu
        ne estis dis<tld/>ita de la napoleona armeo
        <fnt>
          <lok><url ref="https://andaluziaesperanto.wordpress.com/ekskursoj/">Andaluzia Esperanto
          2015</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>    
  <trd lng="de">ausrauben</trd>
  <trdgrp lng="en">
    <trd>to <ind>ransack</ind></trd>,
    <trd>to <ind>plunder</ind></trd>,
    <trd>to <ind>pillage</ind></trd>,
    <trd>to <ind>loot</ind></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7565;&#x596A;&#x3059;&#x308B;  [<ind>&#x308A;&#x3083;&#x304F;&#x3060;&#x3064;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x8352;&#x3089;&#x3059;  [<ind>&#x3042;&#x3089;&#x3059;</ind>]</trd>,
    <trd>&#x596A;&#x3063;&#x3066;&#x5C71;&#x5206;&#x3051;&#x3059;&#x308B;  [<ind>&#x3046;&#x3070;&#x3063;&#x3066;&#x3084;&#x307E;&#x308F;&#x3051;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="pl">rozkra&sacute;&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_g;&c_r;&c_a;&c_b;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_g;&c_r;&c_a;&c_b;&c_l;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&#x43E;&c_z;&c_g;&c_r;&c_a;&c_b;&#x43E;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&#x43E;&c_z;&#x43A;&c_r;&c_a;&c_d;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>


<drv mrk="rab.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="pren.for0i">Forpreni</ref>, kiel <tld/>a&jcirc;on:
      <ekz>
        iliaj kameloj estos for<tld/>itaj, kaj la multo de iliaj 
        brutoj fari&gcirc;os <tld/>akiro
	<fnt><bib>MT</bib>, <lok>&Jer; 49:32</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        for<tld/>i la justecon de senhavuloj
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 10:2</lok>
        </fnt>;
      </ekz>
      <ekz>
