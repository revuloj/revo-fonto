<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: eksa.xml,v 1.4 2023/11/07 19:22:36 revo Exp $">
<kap>
    <rad>eksa</rad>- <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="eksa.0">
  <kap><tld/>-</kap>
  <mlg>E</mlg>
  <gra><vspec>prefikso</vspec></gra>
  <snc mrk="eksa.0.MEZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      &leftquot;Triiliono da&rightquot;, 10<sup>18</sup>
      en la Internacia Sistemo de unuoj (SI):
      <ekz>
        10<sup>18</sup> W &dash; EW &dash; <tld/>vato
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=27382">Vato (mezurunuo)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        1 <tld/>metro egalas al <klr>[...]</klr> 110 lumjaroj (proksimume)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=39626">1 E18 m</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        grando de subdiskoj &gcirc;is 1024 petabajtoj (unu <tld/>bajto)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=133002">Ext4</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_k;&c_s;&c_a;-</trd>
  <trd lng="bg">&c_je;&c_k;&c_s;&c_a;-</trd>
  <trd lng="ca">exa</trd>
  <trd lng="de">Exa-</trd>
  <trd lng="en">exa-</trd>
  <trd lng="es">exa-</trd>
  <trd lng="fa">&a_A;&f_g;&a_z;&a_A;</trd>
  <trd lng="fr">exa-</trd>
  <trd lng="hu">exa-</trd>
  <trd lng="it">exa-</trd>
  <trd lng="nl">exa-</trd>
  <trd lng="no">exa-</trd>
  <trd lng="pl">eksa-</trd>
  <trd lng="pt">exa</trd>
  <trd lng="ru">&c_e;&c_k;&c_s;&c_a;-</trd>
  <trd lng="sv">exa-</trd>
  <trd lng="zh">&#x827E;&#x53EF;&#x8428; <pr>&#xE0;ik&#x11B;s&#xE0;</pr></trd>
</drv>
