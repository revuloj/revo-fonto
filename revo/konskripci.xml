<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: konskripci.xml,v 1.5 2019/10/13 15:10:15 revo Exp $">
<kap>
  <rad>konskripci</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="konskripci.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="konskripci.0i.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="dif" cel="rekrut.0igi.MIL">Rekrutigi</ref>
      por deviga militservo:
      <ekz>
        venis kompanio de soldatoj el la &ccirc;efsidejo de <nom>&Ubreve;ang</nom> 
        la Leono por <tld/>i niajn vila&gcirc;anojn
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>;
      </ekz>
      <ekz>
        li subtenis <klr>[&#8230;]</klr> <tld/>i &ccirc;iujn korposanajn virojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=21115">Francis Lubbock</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="varb.0i.MIL">varbi<sncref/></ref>
  </snc>
  <!-- formon maloftan traduku per malofta&jcirc;o se tia ekzistas -->
  <trdgrp lng="cs">
    <trd>konskribovat</trd>,
    <trd>sepsat</trd>
  </trdgrp> 
  <trdgrp lng="de">
    <trd>konskribieren</trd>,
    <trd>einziehen <klr>(zum Armeedienst)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pob&oacute;r wykona&cacute;</trd>,
    <trd>rekrutowa&cacute;</trd>,
    <trd>werbowa&cacute;</trd>
  </trdgrp> 
  <trd lng="ro">recrutare</trd>
  <trdgrp lng="sk">
    <trd>konskribova&tcaron;</trd>,
    <trd>sp&iacute;sa&tcaron;</trd>
  </trdgrp>
</drv>
</art>

