<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>recenz</rad>/i
</kap>
<drv mrk="recenz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari en gazeto mallongan kritikan raporton pri iu verko, spektaklo
      a&ubreve; ludo, montrante la kvalitojn kaj mankojn.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>rezensieren</trd>,
    <trd>besprechen</trd>
  </trdgrp>
  <trd lng="en">review</trd>
  <trdgrp lng="nl">
    <trd>recenseren</trd>,
    <trd>bespreken</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_c;&c_je;&c_n;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
