<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: recenz.xml,v 1.23 2023/10/20 19:20:37 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>recenz</rad>/i
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="recenz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari en publika&jcirc;o mallongan kritikan raporton pri iu verko,
      spekta&jcirc;o, ludo, montrante kvalitojn kaj mankojn:
      <ekz>
        <nom>B&ouml;&ouml;k</nom>, kiu pli frue <tld/>is liajn verkojn sufi&ccirc;e bonvole, subite mortigis
        romanon, pri kiu <nom>Berger</nom> havis grandan esperon
        <fnt>
          <bib>Kiso</bib>
          <lok>Tombeja Arabesko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>ata libro aperis origine en la itala
        <fnt>
          <aut>Boris Kolker</aut>:
          <vrk>&Ccirc;u la perfekta lingvo troveblas? (Umberto Eco,
          La ser&ccirc;ado de la perfekta lingvo)</vrk>,
          <bib>Monato</bib><lok>jaro 1997a, numero 2a, p. 22a</lok>
        </fnt>;
      </ekz>
      <ekz>
        nia &ccirc;efkuiristino <klr>[&#8230;]</klr> mem testi<klr>[s]</klr> la diversajn receptojn
        <klr>[&#8230;]</klr>, ideale, iu ajn, kiu kompilas a&ubreve; <tld/>as libron pri la
        kuirarto, devus sekvi la la&ubreve;dindan principon de <nom>Maria</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="&Monato;/2003/008020.html"
              >Man&gcirc;u por sano, samideano!</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu <ind><tld/>anto</ind> de Monato rifuzis <tld/>i la diskojn
        <fnt>
          <bib>Monato</bib>
          <aut>pp</aut>
          <vrk><url ref="&Monato;/2005/008638.php"
              >Rava a&ubreve; ruba Ralph?</url></vrk>
          <lok>2005</lok>
        </fnt>.
        <trd lng="de">Rezensent</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kritik.0i.ART">kritiki<sncref/></ref>,
      <ref cel="diskut.pri0i">pridiskuti</ref>,
      <ref cel="revizi.0i.verkon">revizii<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_c;&c_e;&c_n;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>posuzovat</trd>,
    <trd>recenzovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>rezensieren</trd>,
    <trd>besprechen</trd>
  </trdgrp>
  <trd lng="en">review</trd>
  <trdgrp lng="es">
    <trd>rese&ntilde;ar</trd>,
    <trd>escribir una <ind>cr&iacute;tica</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire la <ind>critique</ind> de</trd>,
    <trd>critiquer<klr> (un ouvrage)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ismertet <klr tip="amb">(pl. k&ouml;nyvet, filmet)</klr></trd>,
    <trd>kritik&aacute;t &iacute;r</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#25209;&#35413;&#12377;&#12427;  <pr>&#12402;&#12402;&#12423;&#12358;&#12377;&#12427;</pr></trd>,
    <trd>&#35542;&#35413;&#12377;&#12427;  <pr>&#12429;&#12435;&#12404;&#12423;&#12358;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>recenseren</trd>,
    <trd>bespreken</trd>
  </trdgrp>
  <trd lng="pl">recenzowa&cacute;</trd>
  <trd lng="ru">&c_r;&c_je;&c_c;&c_je;&c_n;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>posudzova&tcaron;</trd>,
    <trd>recenzova&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_r;&c_je;&c_c;&c_je;&c_n;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="recenz.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Kritika <ref cel="raport.0o">raporteto</ref> pri verko, spekta&jcirc;o:
      <ekz>
        <tld/>o pri la verko Rimportretoj de <nom>K. Kalocsay</nom>
        <fnt>
          <bib>LSB</bib>
          <lok>La Forgesita Rimportreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i tie la a&ubreve;toroj eraras, evidente, ne vidinte la spektaklon mem, kaj ju&gcirc;ante
        nur la&ubreve; <tld/>o
        <fnt>
          <bib>KFl</bib>
          <lok>VIII. &Scirc;tupoj al idealo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi &gcirc;is nun neniam reagis al <tld/>o, sed mi pli profitis, mi
        kredas, de kritikoj ol de la&ubreve;doj
        <fnt>
          <aut>Trevor Steele</aut>:
          <vrk>Paranoja reago (1)</vrk>,
          <bib>Monato</bib><lok>jaro 2001a, numero 4a, p. 4a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kritik.0o.ART">kritiko<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_c;&c_e;&c_n;&c_z;&c_ib;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>posudek</trd>,
    <trd>recenze</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Rezension</trd>,
    <trd>Besprechung</trd>
  </trdgrp>
  <trd lng="en">review</trd>
  <trdgrp lng="es">
    <trd>rese&ntilde;a</trd>,
    <trd>cr&iacute;tica <klr>(de una publicaci&oacute;n o espect&aacute;culo)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>critique<klr> (article critique)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kritika</trd>,
    <trd>recenzi&oacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#26360;&#35413;  <pr>&#12375;&#12423;&#12402;&#12423;&#12358;</pr></trd>,
    <trd>&#25209;&#35413;  <pr>&#12402;&#12402;&#12423;&#12358;</pr></trd>,
    <trd>&#35542;&#35413;  <pr>&#12429;&#12435;&#12404;&#12423;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">recensie</trd>
  <trd lng="pl">recenzja</trd>
  <trd lng="ru">&c_r;&c_je;&c_c;&c_je;&c_n;&c_z;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>posudok</trd>,
    <trd>recenzia</trd>
  </trdgrp>
  <trd lng="uk">&c_r;&c_je;&c_c;&c_je;&c_n;&c_z;&c_ib;&c_ja;</trd>
</drv>

<drv mrk="recenz.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Homo, kiu legas, spektas, a&ubreve;skultas novajn publika&jcirc;ojn
      kaj raportadas pri ili:
      <ekz>
        Tre konata <tld/>isto &parsl; Skribis foje en <tld/>on &parsl;
        Sa&gcirc;an frazon, kies sencon &parsl; Fu&scirc;kripligis la presisto
        <fnt>
          <bib>LSB</bib>
          <lok>Alia Epigramo</lok>
        </fnt>;
      </ekz>
      <ekz>
        eble, kiel konjektis unu <tld/>isto, la unua &ccirc;apitro estas tute
        fikcia
        <fnt><aut>Donald Broadribb</aut>:
        <vrk>&leftquot;Fikciaj&rightquot; epizodoj el eble nefikcia vivo</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 4a, p. 20a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_c;&c_e;&c_n;&c_z;&c_e;&c_n;&c_t;</trd>
  <trd lng="cs">recenzent</trd>
  <trd lng="de">Rezensent</trd>
  <trd lng="en">reviewer</trd>
  <trd lng="es">cr&iacute;tico <klr>(de publicaciones o
  espect&aacute;culos)</klr></trd>
  <trdgrp lng="fr">
    <trd>critique litt&eacute;raire</trd>
  </trdgrp>
  <trd lng="hu">kritikus</trd>
  <trdgrp lng="ja">
    <trd>&#25209;&#35413;&#23478;  <pr>&#12402;&#12402;&#12423;&#12358;&#12363;</pr></trd>,
    <trd>&#26360;&#35413;&#23478;  <pr>&#12375;&#12423;&#12402;&#12423;&#12358;&#12363;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>recensent</trd>,
    <trd>criticus</trd>
  </trdgrp>
  <trd lng="pl">recenzent</trd>
  <trd lng="ru">&c_r;&c_je;&c_c;&c_je;&c_n;&c_z;&c_je;&c_n;&c_t;</trd>
  <trd lng="sk">recenzent</trd>
  <trd lng="uk">&c_r;&c_je;&c_c;&c_je;&c_n;&c_z;&c_je;&c_n;&c_t;</trd>
</drv>
</art>
