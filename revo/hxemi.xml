<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>*</ofc>
    <rad>&hcirc;emi</rad>/o
</kap>
<drv mrk="hxemi.0o">
    <kap><ofc>*</ofc><tld/>o</kap>
    <snc mrk="hxemi.0o.SCI">
      <uzo tip="fak">KEM</uzo>
      <uzo tip="fak">SCI</uzo>
      <dif>
	Scienco studanta per analizo kaj sintezo la konsiston de la
	materiaj substancoj kaj la fenomenojn &ccirc;e kiuj
	&scirc;an&gcirc;i&gcirc;as la interna strukturo de la korpoj.
      </dif>
    </snc>
    <trd lng="be">&c_h;&c_ib;&c_m;&c_ib;&c_ja;</trd>
    <trdgrp lng="de">
      <trd>Chemie</trd>,
      <trd>Scheidekunst<klr> [ark]</klr></trd>
    </trdgrp>
    <trd lng="ca">qu&iacute;mica</trd>
    <trd lng="fi">kemia</trd>
    <trd lng="en">chemistry</trd>
    <trd lng="es">qu&iacute;mica</trd>
    <trd lng="fr">chimie</trd>
    <trdgrp lng="hu">
      <trd>k&eacute;mia</trd>,
      <trd>vegytan</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>chemie</trd>,
      <trd>scheikunde</trd>
    </trdgrp>
    <trd lng="pl">chemia</trd>
    <trd lng="pt">qu&iacute;mica<klr tip="amb"> 
(ci&ecirc;ncia)</klr></trd>
    <trd lng="ru">&c_h;&c_i;&c_m;&c_i;&c_ja;</trd>
    <trd lng="sk">ch&eacute;mia</trd>
    <trd lng="sv">kemi</trd>
    <trd lng="vo">kiem</trd>
    <trd lng="it">chimica</trd>
</drv>

<drv mrk="hxemi.0a">
    <kap><tld/>a</kap>
    <snc mrk="hxemi.0a.SCI">
      <dif>
	Rilata al <tld/>o:
	<ekz>
	  <tld/>a armilo, bombo;
	</ekz>
	<ekz>
	  multajn sanproblemojn ili povus solvi sen <tld/>aj
	  kuraciloj
	  <fnt>
	  <aut>Laimius STRA&Zcaron;NICKAS</aut>,
	  <vrk>Verdaj kuraciloj</vrk>,
	  <bib>Monato</bib>.
	  </fnt>.
	</ekz>
      </dif>
    </snc>
    <trd lng="de">chemisch</trd>
    <trd lng="fi">kemiallinen</trd>
    <trd lng="fr">chimique</trd>
    <trd lng="hu">k&eacute;miai</trd>
    <trd lng="pl">chemiczny</trd>
    <trd lng="pt">qu&iacute;mico<klr tip="ind"> (adj.)</klr></trd>
    <trd lng="ru">
&c_h;&c_i;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
    <trd lng="vo">kiemik</trd>
    <trd lng="sv">kemisk</trd>
    <trd lng="it">chimico<klr> (agg.)</klr></trd>
</drv>

<drv mrk="hxemi.0ajxo">
    <kap><tld/>a&jcirc;o</kap>
    <snc mrk="hxemi.0ajxo.substanco">
      <dif>
	<tld lit="&Hcirc;"/>a substanco, precipe ia uzata en <tld/>aj
	eksperimentoj a&ubreve; analizoj:
	<ekz>
	  Milk elportis rakon de analizaj <tld/>a&jcirc;oj kaj
	  la pulvorigilon
	  <fnt>
	  <aut>&Gcirc;ak VANS</aut>,
	  <vrk><url ref="http://esperanto.us/TrigambaJocxjo.html"
	  >Trigamba Jo&ccirc;jo</url></vrk>
	  </fnt>
	</ekz>
      </dif>
      <trdgrp lng="pl">
	<trd>substancja chemiczna</trd>,
	<trd>&sacute;rodek chemiczny</trd>,
	<trd>chemikalia<klr> (l.mn.)</klr></trd>
      </trdgrp>
      <trdgrp lng="ru">
	<trd>&c_h;&c_i;&c_m;&c_i;&c_k;&c_a;&c_l;&c_i;&c_j;</trd>,
	<trd>&c_r;&c_je;&c_a;&c_k;&c_t;&c_i;&c_v;</trd>,
	<trd>&c_h;&c_i;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je;
&c_v;&c_je;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
      </trdgrp>
    </snc>
    <snc mrk="hxemi.0ajxo.produkto">
      <dif>
	<tld lit="&Hcirc;"/>a produkto, varo produktita per <tld/>aj
	metodoj:
	<ekz>
	  pro agrokulturaj <tld/>a&jcirc;oj lampiroj malaperis, sed
	  nun multaj vilagxoj ilin kultivas por allogi turistojn
	  <fnt>
	  <aut>HORI Yasuo</aut>, <vrk>Japanio</vrk>, <bib>Monato</bib>.
	  </fnt>.
	</ekz>
      </dif>
      <trdgrp lng="ru">
	<trd>&c_h;&c_i;&c_m;&c_i;&c_k;&c_a;&c_t;</trd>,
	<trd>&c_h;&c_i;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je;
&c_i;&c_z;&c_d;&c_je;&c_l;&c_i;&c_je;</trd>,
	<trd>&c_h;&c_i;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;
&c_p;&c_r;&c_je;&c_p;&c_a;&c_r;&c_a;&c_t;</trd>
      </trdgrp>
    </snc>
    <trdgrp lng="be">
      <trd>&c_h;&c_ib;&c_m;&c_ib;&c_k;&c_a;&c_t;</trd>,
      <trd>&c_h;&c_ib;&c_m;&c_ib;&c_k;&c_a;&c_l;&c_ib;&c_ib;</trd>,
      <trd>&c_h;&c_ib;&c_m;&c_ib;&c_ch;&c_n;&c_a;&c_je;
&c_r;&c_e;&c_ch;&c_y;&c_v;&c_a;</trd>
    </trdgrp>
    <trd lng="ca">producte <ind>qu&iacute;mic</ind></trd>
    <trd lng="de">Chemikalie</trd>
    <trd lng="en">chemical</trd>
    <trd lng="fi">kemikaali</trd>
    <trd lng="es">producto <ind>qu&iacute;mico</ind></trd>
    <trdgrp lng="fr">
      <trd>produit chimique</trd>,
      <trd>substance chimique</trd>
    </trdgrp>
    <trd lng="hu">vegyszer</trd>
    <trdgrp lng="nl">
      <trd>chemieproduct</trd>,
      <trd>chemicali&euml;n<klr> (= &hcirc;emia&jcirc;oj)</klr></trd>
    </trdgrp>
    <trd lng="pl">&sacute;rodek chemiczny</trd>
    <trd lng="pt">produto <ind>qu&iacute;mico</ind></trd>
    <trd lng="sk">chemik&aacute;lia</trd>
    <trd lng="sv">kemikalie</trd>
    <trdgrp lng="it">
      <trd>prodotto chimico</trd>,
      <trd>sostanza chimica</trd>
    </trdgrp>
</drv>

<drv mrk="hxemi.0isto">
    <kap><ofc>*</ofc><tld/>o</kap>
    <snc>
      <uzo tip="fak">KEM</uzo>
      <uzo tip="fak">SCI</uzo>
      <dif>
      Scienzulo a&ubreve; teknikisto pri 
      <ref tip="dif" cel="hxemi.0o">&hcirc;emio</ref>.
      </dif>
     </snc>
    <trd lng="it">chimico<klr> (sost.)</klr></trd>
   </drv>

<drv mrk="hxemi.elektro0o">
    <kap>elektro<tld/>o</kap>
    <snc mrk="hxemi.elektro0o.SCI">
      <uzo tip="fak">KEM</uzo>
      <uzo tip="fak">SCI</uzo>
      <dif>
	Bran&ccirc;o de 
       <ref tip="malprt" cel="hxemi.0o">&hcirc;emio</ref>,
	kiu pritraktas sistemojn kaj interfazajn limojn dum fluo
	de <ref tip="vid" cel="elektr.0o">elektro</ref> tra ili.
      </dif>
    </snc>
    <trd lng="be">
&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_a;&c_h;&c_ib;&c_m;&c_ib;&c_ja;
</trd>
    <trd lng="de">Elektrochemie</trd>
    <trd lng="es">electroqu&iacute;mica</trd>
    <trd lng="fi">s&auml;hk&ouml;kemia</trd>
    <trd lng="fr">&eacute;lectrochimie</trd>
    <trd lng="hu">elektrok&eacute;mia</trd>
    <trd lng="pl">elektrochemia</trd>
    <trd lng="pt">eletroqu&iacute;mica</trd>
    <trd lng="ru">
&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_h;&c_i;&c_m;&c_i;&c_ja;</trd>
    <trd lng="ru">
&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_h;&c_i;&c_m;&c_i;&c_ja;</trd>
    <trd lng="it">elettrochimica</trd>
</drv>

<drv mrk="hxemi.elektro0a">
    <kap>elektro<tld/>a</kap>
    <snc mrk="hxemi.elektro0a.SCI">
      <dif>
	Rilata al &hcirc;emia transformado de substancoj per elektra fluo.
      </dif>
    </snc>
    <trd lng="be">
&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_a;&c_h;&c_ib;&c_m;&c_ib;&c_ch;&c_n;&c_y;
</trd>
    <trd lng="de">elektrochemisch</trd>
    <trd lng="fi">s&auml;hk&ouml;kemiallinen</trd>
    <trd lng="fr">&eacute;lectrochimique</trd>
    <trd lng="hu">elektrok&eacute;miai</trd>
    <trd lng="pl">elektrochemiczny</trd>
    <trd lng="ru">
&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_h;&c_i;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;
</trd>
   <trd lng="it">elettrochimico<klr> (agg.)</klr></trd>
</drv>

<drv mrk="hxemi.imuno0o">
    <kap>imuno<tld/>o</kap>
    <snc mrk="hxemi.imuno0o.SCI">
      <uzo tip="fak">BAK</uzo>
      <uzo tip="fak">SCI</uzo>
      <dif>
	Scienco pri la <tld/>aj strukturo kaj ecoj de la antikorpoj
	kaj antigenoj, kaj pri ilia interagado:
	<ekz>
	  li interesi&gcirc;is pri imunologio, kiam ankora&ubreve; junis
	  tiu &ccirc;i fako, kaj li akiris sciojn pri <tld/>o
	  kaj imuno<tld/>o
	  <fnt>
	  <aut>Ste&ccirc;jo NORVELL</aut>,
	  <vrk>Profeto sen honoro</vrk>,
	  <bib>Monato</bib>.
	  </fnt>.
	</ekz>
      </dif>
    </snc>
    <trd lng="de">Immunchemie</trd>
    <trd lng="fi">immunokemia</trd>
    <trd lng="fr">immunochimie</trd>
    <trd lng="pl">immunolgia</trd>
    <trd lng="ru">
&c_i;&c_m;&c_m;&c_u;&c_n;&c_o;&c_h;&c_i;&c_m;&c_i;&c_ja;</trd>
    <trd lng="it">immunologia</trd>
</drv>

<drv mrk="hxemi.petrol0o">
    <kap>petrol<tld/>o</kap>
    <snc mrk="hxemi.petrol0o.SCI">
      <uzo tip="fak">KEM</uzo>
      <uzo tip="fak">SCI</uzo>
      <dif>
	Bran&ccirc;o de <ref tip="malprt" cel="hxemi.0o">&hcirc;emio</ref>
	studanta konsiston kaj <tld/>ajn ecojn de
	<ref cel="naft.0o">nafto</ref>, <ref
	cel="petrol.0o.rafinita">petroloj</ref>
	kaj natura gaso (gasaj hidrokarbona&jcirc;oj).
      </dif>
    </snc>
    <trd lng="de">Petrochemie</trd>
    <trd lng="en">petrochemistry</trd>
    <trd lng="fi">petrokemia</trd>
    <trd lng="fr">p&eacute;trochimie</trd>
    <trd lng="ru">
&c_n;&c_je;&c_f;&c_t;&c_je;&c_h;&c_i;&c_m;&c_i;&c_ja;</trd>
     <trd lng="it">petrolchimica</trd>
</drv>
</art>
