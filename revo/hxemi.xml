<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc><rad>&hcirc;emi</rad>/o <fnt><bib>PV</bib></fnt>,
  <var><kap><ofc>8</ofc><rad>kemi</rad>/o <fnt><bib>PV</bib></fnt></kap></var>
</kap>
<drv mrk="hxemi.0o">
  <kap><ofc>*</ofc><tld/>o, <var><kap>kemio</kap></var></kap>
  <snc mrk="hxemi.0o.SCI">
    <uzo tip="fak">KEM</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="lst" lst="voko:sciencoj" cel="scienc.0o.fako">Scienco</ref>
      studanta per analizo kaj sintezo la konsiston de la
      materiaj substancoj kaj la fenomenojn &ccirc;e kiuj
      &scirc;an&gcirc;i&gcirc;as la interna strukturo de la korpoj:
      <ekz>
        kiu okupas sin je <tld/>o, estas <tld/>isto
        <fnt>
          <bib>F</bib>
          <lok>&FE; 32</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvar israelanoj ricevis dum la lastaj 10 jaroj Nobel-premiojn pri <tld/>o kaj
        ekonomiko
        <fnt>
          <bib>Monato</bib>
          <aut>Josef &Scirc;emer</aut>
          <vrk><url ref="https://www.monato.be/2008/009524.php">La juda &scirc;tato 60-jara</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        anka&ubreve; kemio estas arabdevena vorto
        <fnt>
          <bib>Monato</bib>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006903.html">Pruntoj: Arabaj vortoj en la germana kaj esperanta lingvoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_ib;&c_m;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_h;&c_i;&c_m;&c_i;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Chemie</trd>,
  </trdgrp>
  <trd lng="ca">qu&iacute;mica</trd>
  <trd lng="en">chemistry</trd>
  <trd lng="es">qu&iacute;mica</trd>
  <trd lng="fa">&a_sh;&f_y;&a_m;&f_y;</trd>
  <trd lng="fi">kemia</trd>
  <trd lng="fr">chimie</trd>
  <trdgrp lng="hu">
    <trd>k&eacute;mia</trd>,
    <trd>vegytan</trd>
  </trdgrp>
  <trd lng="id">kimia</trd>
  <trd lng="it">chimica</trd>
  <trdgrp lng="nl">
    <trd>chemie</trd>,
    <trd>scheikunde</trd>
  </trdgrp>
  <trd lng="no">kjemi</trd>
  <trd lng="pl">chemia</trd>
  <trd lng="pt">qu&iacute;mica
    <klr tip="amb">(ci&ecirc;ncia)</klr></trd>
  <trd lng="ru">&c_h;&c_i;&c_m;&c_i;&c_ja;</trd>
  <trd lng="sk">ch&eacute;mia</trd>
  <trd lng="sv">kemi</trd>
  <trd lng="vo">kiem</trd>
</drv>

<drv mrk="hxemi.0a">
  <kap><tld/>a, <var><kap>kemia</kap></var></kap>
  <snc mrk="hxemi.0a.SCI">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a armilo, bombo;
      </ekz>
      <ekz>
        elpensa&jcirc;o, kiun oni nomos <tld/>aj alumetoj
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        estus terura <klr>[&#8230;]</klr> la <tld/>a kuni&gcirc;o de la densigita vosto de la kometo
        kun la oksigeno de nia atmosfero
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        multajn sanproblemojn ili povus solvi sen <tld/>aj
        kuraciloj
        <fnt>
          <aut>L. Stra&zcaron;nickas</aut>,
          <vrk>Verdaj kuraciloj</vrk>,
          <bib>Monato</bib>.
        </fnt>;
      </ekz>
      <ekz>
        enzimoj ebligas kaj regas la sennombrajn kemiajn reagojn kiuj estas necesaj por
        vivo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007157.html">Vivo: Riboj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_ib;&c_m;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="de">chemisch</trd>
  <trd lng="en">chemical</trd>
  <trd lng="fa">&a_sh;&f_y;&a_m;&f_y;&a_A;&f_y;&f_y;</trd>
  <trd lng="fi">kemiallinen</trd>
  <trd lng="fr">chimique</trd>
  <trd lng="hu">k&eacute;miai</trd>
  <trd lng="it">chimico<klr> (agg.)</klr></trd>
  <trdgrp lng="nl">
    <trd>chemisch</trd>,
    <trd>scheikundig</trd>
  </trdgrp>
  <trd lng="no">kjemisk</trd>
  <trd lng="pl">chemiczny</trd>
  <trd lng="pt">qu&iacute;mico<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="ru">&c_h;&c_i;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="vo">kiemik</trd>
  <trd lng="sv">kemisk</trd>
</drv>

<drv mrk="hxemi.0ajxo">
  <kap><tld/>a&jcirc;o, <var><kap>kemia&jcirc;o</kap></var></kap>
  <snc mrk="hxemi.0ajxo.substanco">
    <dif>
      <tld lit="&Hcirc;"/>a substanco, precipe ia uzata en <tld/>aj
      eksperimentoj a&ubreve; analizoj:
      <ekz>
        <nom>Milk</nom> elportis rakon de analizaj <tld/>a&jcirc;oj kaj
        la pulvorigilon
        <fnt>
          <aut>&Gcirc;. Vans</aut>,
          <vrk><url ref="http://esperanto.us/TrigambaJocxjo.html">Trigamba Jo&ccirc;jo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Usono e&ccirc; liveris la <tld/>ajn kaj biologiajn armilojn, per kiuj Irako tiutempe
        povis batali
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="https://www.monato.be/2003/008061.html">Usono inter Irano kaj Irako</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        multaj preferas kutimi&gcirc;i al la sendan&gcirc;eraj kuloj anstata&ubreve; uzi kemia&jcirc;ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007162.html">&Gcirc;ojoj kaj &gcirc;uoj de finna somero</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>substancja chemiczna</trd>,
      <trd>&sacute;rodek chemiczny</trd>,
      <trd>chemikalia<klr> (l.mn.)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_h;&c_i;&c_m;&c_i;&c_k;&c_a;&c_l;&c_i;&c_j;</trd>,
      <trd>&c_r;&c_je;&c_a;&c_k;&c_t;&c_i;&c_v;</trd>,
      <trd>&c_h;&c_i;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; &c_v;&c_je;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
    </trdgrp>
  </snc>
  <snc mrk="hxemi.0ajxo.produkto">
    <dif>
      <tld lit="&Hcirc;"/>a produkto, varo produktita per <tld/>aj
      metodoj:
      <ekz>
        pro agrokulturaj <tld/>a&jcirc;oj lampiroj malaperis, sed
        nun multaj vila&gcirc;oj ilin kultivas por allogi turistojn
        <fnt>
        <aut>Hori Y.</aut>, <vrk>Japanio</vrk>, <bib>Monato</bib>.
        </fnt>;
      </ekz>
      <ekz>
        ma&scirc;ino kiu purigas paperon, forprenante la inkon <klr>[&#8230;]</klr> per <tld/>a&jcirc;oj
        kaj varmo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9310-artikoletoj.txt">Malkopiilo</url></vrk>
          <lok>93/10</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_h;&c_i;&c_m;&c_i;&c_k;&c_a;&c_t;</trd>,
      <trd>&c_h;&c_i;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; &c_i;&c_z;&c_d;&c_je;&c_l;&c_i;&c_je;</trd>,
      <trd>&c_h;&c_i;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; &c_p;&c_r;&c_je;&c_p;&c_a;&c_r;&c_a;&c_t;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_ib;&c_m;&c_ib;&c_k;&c_a;&c_t;</trd>,
    <trd>&c_h;&c_ib;&c_m;&c_ib;&c_k;&c_a;&c_l;&c_ib;&c_ib;</trd>,
    <trd>&c_h;&c_ib;&c_m;&c_ib;&c_ch;&c_n;&c_a;&c_je; &c_r;&c_e;&c_ch;&c_y;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">producte <ind>qu&iacute;mic</ind></trd>
  <trd lng="de">Chemikalie</trd>
  <trd lng="en">chemical</trd>
  <trd lng="es">producto <ind>qu&iacute;mico</ind></trd>
  <trd lng="fa">&a_m;&a_A;&a_d;&a_h;&a_hamza_sure; <ind>&a_sh;&f_y;&a_m;&f_y;&a_A;&f_y;&f_y;</ind></trd>
  <trd lng="fi">kemikaali</trd>
  <trdgrp lng="fr">
    <trd>produit chimique</trd>,
    <trd>substance chimique</trd>
  </trdgrp>
  <trd lng="hu">vegyszer</trd>
  <trdgrp lng="it">
    <trd>prodotto chimico</trd>,
    <trd>sostanza chimica</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>chemieproduct</trd>,
    <trd>chemicali&euml;n<klr> (= &hcirc;emia&jcirc;oj)</klr></trd>
  </trdgrp>
  <trd lng="no">kjemikalie</trd>
  <trd lng="pl">&sacute;rodek chemiczny</trd>
  <trd lng="pt">produto <ind>qu&iacute;mico</ind></trd>
  <trd lng="sk">chemik&aacute;lia</trd>
  <trd lng="sv">kemikalie</trd>
</drv>

<drv mrk="hxemi.0isto">
  <kap><ofc>*</ofc><tld/>isto, <var><kap>kemiisto</kap></var></kap>
  <snc mrk="hxemi.0isto.SCI">
    <uzo tip="fak">KEM</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Scienculo a&ubreve; te&hcirc;nikisto pri 
      <ref tip="vid" cel="hxemi.0o.SCI">&hcirc;emio</ref>:
      <ekz>
        viva ekzista&jcirc;o ne povas esti kreita en la retorto de <tld/>isto
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>isto <nom>Kunz</nom> konfirmas la supozojn de <nom>Boyl</nom>
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        li ne estis <tld/>isto, astronomo, matematikisto
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        kongreso de kemiistoj
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="https://www.monato.be/2009/009858.php">La mondrevolucia verko finfine en Esperanto</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_ib;&c_m;&c_ib;&c_k;</trd>
  <trd lng="de">Chemiker</trd>
  <trd lng="en">chemist</trd>
  <trd lng="fa">&a_sh;&f_y;&a_m;&f_y;&zwnj;&a_d;&a_A;&a_n;</trd>
  <trd lng="fr">chimiste</trd>
  <trd lng="it">chimico<klr> (sost.)</klr></trd>
  <trd lng="id">kimiawan</trd>
  <trd lng="pl">chemik</trd>
</drv>

<drv mrk="hxemi.elektro0o">
  <kap>elektro<tld/>o</kap>
  <snc mrk="hxemi.elektro0o.SCI">
    <uzo tip="fak">KEM</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Bran&ccirc;o de 
      <ref tip="malprt" cel="hxemi.0o.SCI">&hcirc;emio</ref>,
      kiu pritraktas sistemojn kaj interfazajn limojn dum fluo
      de <ref tip="vid" cel="elektr.0o">elektro</ref> tra ili:
      <ekz>
        por elektro<tld/>o gravas la fakto, ke la surfaco de elektrodoj el la
        <klr>[vitra]</klr> grafito estas tre &hcirc;emie imuna
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=46676">Grafito</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_a;&c_h;&c_ib;&c_m;&c_ib;&c_ja;</trd>
  <trd lng="de">Elektrochemie</trd>
  <trd lng="en">electrochemistry</trd>
  <trd lng="es">electroqu&iacute;mica</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_r;&a_q;&a_A;&a_sh;&f_y;&a_m;&f_y;</trd>,
    <trd>&a_A;&a_l;&f_k;&a_t;&a_r;&a_w;&a_sh;&f_y;&a_m;&f_y;</trd>
  </trdgrp>
  <trd lng="fi">s&auml;hk&ouml;kemia</trd>
  <trd lng="fr">&eacute;lectrochimie</trd>
  <trd lng="hu">elektrok&eacute;mia</trd>
  <trd lng="id">elektrokimia</trd>
  <trd lng="it">elettrochimica</trd>
  <trd lng="nl">elektrochemie</trd>
  <trd lng="no">elektrokjemi</trd>
  <trd lng="pl">elektrochemia</trd>
  <trd lng="pt">eletroqu&iacute;mica</trd>
  <trd lng="ru">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_h;&c_i;&c_m;&c_i;&c_ja;</trd>
</drv>

<drv mrk="hxemi.elektro0a">
  <kap>elektro<tld/>a, <var><kap>elektrokemia</kap></var></kap>
  <snc mrk="hxemi.elektro0a.SCI">
    <dif>
      Rilata al &hcirc;emia transformado de substancoj per elektra fluo:    
      <ekz>
        per elektrokemia procedo la emisiado reali&gcirc;as &ccirc;e ordinara
        temperaturo    
        <fnt>
          <aut>-, trad. J. Palu</aut>,
          <vrk><url ref="http://www.fw.hu/eventoj/steb/kemio/hkk7612.htm">
          Kion kapablas elektrono?</url></vrk>,
          <lok>Homo kaj Kosmo, numero 1976/1-1, p. 7a-8a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_a;&c_h;&c_ib;&c_m;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="de">elektrochemisch</trd>
  <trd lng="en">electrochemical</trd>
  <trd lng="fi">s&auml;hk&ouml;kemiallinen</trd>
  <trd lng="fr">&eacute;lectrochimique</trd>
  <trd lng="hu">elektrok&eacute;miai</trd>
  <trd lng="it">elettrochimico<klr> (agg.)</klr></trd>
  <trd lng="nl">elektrochemisch</trd>
  <trd lng="no">elektrokjemisk</trd>
  <trd lng="pl">elektrochemiczny</trd>
  <trd lng="ru">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_h;&c_i;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="hxemi.imuno0o">
  <kap>imuno<tld/>o</kap>
  <snc mrk="hxemi.imuno0o.SCI">
    <uzo tip="fak">BAK</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Scienco pri la <tld/>aj strukturo kaj ecoj de la antikorpoj
      kaj antigenoj, kaj pri ilia interagado:
      <ekz>
        li interesi&gcirc;is pri imunologio, kiam ankora&ubreve; junis
        tiu &ccirc;i fako, kaj li akiris sciojn pri <tld/>o
        kaj imuno<tld/>o
        <fnt>
          <aut>S. Norvell</aut>,
          <vrk>Profeto sen honoro</vrk>,
          <bib>Monato</bib>.
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_m;&c_u;&c_n;&c_a;&c_h;&c_ib;&c_m;&c_ib;&c_ja;</trd>
  <trd lng="de">Immunchemie</trd>
  <trd lng="en">immunochemistry</trd>
  <trd lng="fi">immunokemia</trd>
  <trd lng="fr">immunochimie</trd>
  <trd lng="it">immunologia</trd>
  <trd lng="nl">immunochemie</trd>
  <trd lng="no">immunokjemi</trd>
  <trd lng="pl">immunolgia</trd>
  <trd lng="ru">&c_i;&c_m;&c_m;&c_u;&c_n;&c_o;&c_h;&c_i;&c_m;&c_i;&c_ja;</trd>
</drv>

<drv mrk="hxemi.neorganika0o">
  <kap>neorganika <tld/>o<ofc>9</ofc></kap>
  <snc mrk="hxemi.neorganika0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Parto de <ref tip="malprt" cel="hxemi.0o.SCI"><tld/>o</ref>
      kiu studas neorganika&jcirc;ojn:
      <ekz>
        &ccirc;i tiuj esta&jcirc;oj <klr>[&#8230;]</klr> konsistas <klr>[&#8230;]</klr> el 
        fero, oro kaj anka&ubreve; el multspecaj metaloj kaj mineraloj,
        inter ili anka&ubreve; el tiaj, kiujn ne konas nia neorganika <tld/>o
        <fnt>
          <aut>F. Karinthy, trad. L. Totsche</aut>,
          <vrk><url ref="http://i-espero.info/files/elibroj/eo%20-%20karinthy,%20frigyes%20-%20vojagxo%20al%20faremido.pdf">
          Voja&gcirc;o al Faremido</url></vrk>,
          <lok>1933</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_h;&c_ib;&c_m;&c_ib;&c_ja;</ind></trd>
  <trd lng="de">anorganische Chemie</trd>
  <trd lng="fr">chimie inorganique</trd>
  <trd lng="id">kimia anorganik</trd>
</drv>

<drv mrk="hxemi.organika0o">
  <kap>organika <tld/>o<ofc>9</ofc></kap>
  <snc mrk="hxemi.organika0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Parto de <ref tip="malprt" cel="hxemi.0o.SCI"><tld/>o</ref>
      kiu studas organika&jcirc;ojn, tio estas precipe substancojn
      kun karbona a&ubreve; hidrogena bazo:
      <ekz>
        organika <tld/>o enhavas ege pli multe da kombina&jcirc;oj ol 
        neorganika <tld/>o
        <fnt>
          <aut>Z. Pluha&rcaron;</aut>,
          <vrk><url ref="http://www.eventoj.hu/steb/kemio/kemia-nomenklaturo-versio-2011-2.pdf">
          Sistema &Hcirc;emia Nomenklaturo en Esperanto</url></vrk>,
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_h;&c_ib;&c_m;&c_ib;&c_ja;</ind></trd>
  <trd lng="de">organische Chemie</trd>
  <trd lng="fr">chimie organique</trd>
  <trd lng="id">kimia organik</trd>
</drv>

<drv mrk="hxemi.petrol0o">
  <kap>petrol<tld/>o</kap>
  <snc mrk="hxemi.petrol0o.SCI">
    <uzo tip="fak">KEM</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
