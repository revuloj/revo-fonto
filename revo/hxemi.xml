<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>ĥemi</rad>/o
    </kap>

    <drv mrk="hxemi.0o">
      <kap>
        <ofc>*</ofc><tld/>o
      </kap>
      <snc>
        <dif>
          Scienco studanta per analizo kaj sintezo la konsiston de la
          materiaj substancoj kaj la fenomenojn ĉe kiuj
          ŝanĝiĝas la interna strukturo de la korpoj.
        </dif>
      </snc>
      <trd lng="be">хімія</trd>
      <trdgrp lng="de">
        <trd>Chemie</trd>,
        <trd>Scheidekunst<klr> [ark]</klr></trd>
      </trdgrp>
      <trd lng="ca">química</trd>
      <trd lng="en">chemistry</trd>
      <trd lng="es">química</trd>
      <trd lng="fr">chimie</trd>
      <trdgrp lng="hu">
        <trd>kémia</trd>,
        <trd>vegytan</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>chemie</trd>,
        <trd>scheikunde</trd>
      </trdgrp>
      <trd lng="pl">chemia</trd>
      <trd lng="pt">química<klr tip="amb"> (ciência)</klr></trd>
      <trd lng="ru">химия</trd>
      <trd lng="sk">chémia</trd>
      <trd lng="sv">kemi</trd>
      <trd lng="vo">kiem</trd>
    </drv>

    <drv mrk="hxemi.0a">
      <kap>
        <tld/>a
      </kap>
      <snc>
        <dif>
          Rilata al <tld/>o:
          <ekz>
            <tld/>a armilo, bombo;
          </ekz><ekz>
            multajn sanproblemojn ili povus solvi sen <tld/>aj
            kuraciloj
            <fnt>
              <aut>Laimius STRAŽNICKAS</aut>:
              <vrk>Verdaj kuraciloj</vrk>
              <bib>Monato</bib>.
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="de">chemisch</trd>
      <trd lng="hu">kémiai</trd>
      <trd lng="pl">chemiczny</trd>
      <trd lng="pt">químico<klr tip="ind"> (adj.)</klr></trd>
      <trd
        lng="ru">химический</trd>
      <trd lng="vo">kiemik</trd>
    </drv>

    <drv mrk="hxemi.0ajxo">
      <kap><tld/>aĵo</kap>
      <snc mrk="hxemi.0ajxo.substanco">
        <dif>
          <tld lit="Ĥ"/>a substanco, precipe ia uzata en <tld/>aj
          eksperimentoj aŭ analizoj:
          <ekz>
            Milk elportis rakon de analizaj <tld/>aĵoj kaj
            la pulvorigilon
            <fnt>
              <aut>Ĝak VANS</aut>:
              <vrk><url ref="http://esperanto.us/TrigambaJocxjo.html"
                  >Trigamba Joĉjo</url></vrk>
            </fnt>
          </ekz>
        </dif>
        <trdgrp lng="pl">
          <trd>substancja chemiczna</trd>
          <trd>środek chemiczny</trd>
          <trd>chemikalia<klr> (l.mn.)</klr></trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>химикалий</trd>,
          <trd>реактив</trd>,
          <trd>химическое вещество</trd>
        </trdgrp>
      </snc>
      <snc mrk="hxemi.0ajxo.produkto">
        <dif>
          <tld lit="Ĥ"/>a produkto, varo produktita per <tld/>aj
          metodoj:
          <ekz>
            pro agrokulturaj <tld/>ajxoj lampiroj malaperis, sed
            nun multaj vilagxoj ilin kultivas por allogi turistojn
	    <fnt>
	      <aut>HORI Yasuo</aut>: <vrk>Japanio</vrk>. <bib>Monato</bib>.
	    </fnt>.
          </ekz>
        </dif>
        <trdgrp lng="ru">
          <trd>химикат</trd>,
          <trd>химическое изделие</trd>,
          <trd>химический препарат</trd>
        </trdgrp>
      </snc>
      <trdgrp lng="be">
        <trd>хімікат</trd>,
        <trd>хімікаліі</trd>,
        <trd>хімічнае рэчыва</trd>
      </trdgrp>
      <trd lng="ca">producte <ind>químic</ind></trd>
      <trd lng="de">Chemikalie</trd>
      <trd lng="en">chemical</trd>
      <trd lng="es">producto <ind>químico</ind></trd>
      <trd lng="fr">substance chimique</trd>
      <trd lng="hu">vegyszer</trd>
      <trdgrp lng="nl">
        <trd>chemieproduct</trd>,
        <trd>chemicaliën<klr> (= ĥemiaĵoj)</klr></trd>
      </trdgrp>
      <trd lng="pl">środek chemiczny</trd>
      <trd lng="pt">produto <ind>químico</ind></trd>
      <trd lng="sk">chemikália</trd>
      <trd lng="sv">kemikalie</trd>
    </drv>

    <drv mrk="hxemi.elektro0o">
      <kap>elektro<tld/>o</kap>
      <snc>
        <dif>
          Branĉo de <ref tip="malprt" cel="hxemi.0o">ĥemio</ref>,
          kiu pritraktas sistemojn kaj interfazajn limojn dum fluo
          de <ref tip="vid" cel="elektr.0o">elektro</ref> tra ili.
        </dif>
      </snc>
      <trd lng="be">электрахімія</trd>
      <trd lng="de">Elektrochemie</trd>
      <trd lng="es">electroquímica</trd>  
      <trd lng="fr">électrochimie</trd>
      <trd lng="hu">elektrokémia</trd>
      <trd lng="pl">elektrochemia</trd>
      <trd lng="pt">eletroquímica</trd>
      <trd lng="ru">электрохимия</trd>
      <trd lng="ru">электрохимия</trd>
    </drv>

    <drv mrk="hxemi.elektro0a">
      <kap>elektro<tld/>a</kap>
      <snc>
        <dif>
          Rilata al ĥemia transformado de substancoj per elektra fluo.  
        </dif>
      </snc>
      <trd lng="be">электрахімічны</trd>
      <trd lng="de">elektrochemisch</trd>
      <trd lng="fr">électrochimique</trd>
      <trd lng="hu">elektrokémiai</trd>
      <trd lng="pl">elektrochemiczny</trd>
      <trd lng="ru">электрохимический</trd>
    </drv>

    <drv mrk="hxemi.imuno0o">
      <kap>
        imuno<tld/>o
      </kap>
      <snc>
        <dif>
          Scienco pri la <tld/>aj strukturo kaj ecoj de la antikorpoj
          kaj antigenoj, kaj pri ilia interagado:
          <ekz>
            li interesiĝis pri imunologio, kiam ankoraŭ junis
            tiu ĉi fako, kaj li akiris sciojn pri <tld/>o
            kaj imuno<tld/>o
            <fnt>
              <aut>Steĉjo NORVELL</aut>: <vrk>Profeto sen honoro</vrk>
              <bib>Monato</bib>.
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="pl">immunolgia</trd>
      <trd lng="ru">иммунохимия</trd>
    </drv>

    <drv mrk="hxemi.petrol0o">
      <kap>petrol<tld/>o</kap>
      <snc>
        <dif>
          Branĉo de <ref tip="malprt" cel="hxemi.0o">ĥemio</ref>
          studanta konsiston kaj <tld/>ajn ecojn de
          <ref cel="naft.0o">nafto</ref>, <ref cel="petrol.o">petroloj</ref>
          kaj natura gaso (gasaj hidrokarbonaĵoj).
        </dif>
      </snc>
      <trd lng="en">petrochemistry</trd>  
      <trd lng="ru">нефтехимия</trd>
    </drv>

  </art>
