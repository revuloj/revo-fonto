<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&hcirc;emi</rad>/o
</kap>
<drv mrk="hxemi.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Scienco studanta per analizo kaj sintezo la konsiston de la materiaj
      substancoj kaj la fenomenojn &ccirc;e kiuj &scirc;an&gcirc;i&gcirc;as la
      interna strukturo de la korpoj.
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_ib;&c_m;&c_ib;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Chemie</trd>,
    <trd>Scheidekunst<klr> [ark]</klr></trd>
  </trdgrp>
  <trd lng="en">chemistry</trd>
  <trd lng="fr">chimie</trd>
  <trdgrp lng="nl">
    <trd>chemie</trd>,
    <trd>scheikunde</trd>
  </trdgrp>
  <trd lng="pt">qu&iacute;mica</trd>
  <trd lng="ru">&c_h;&c_i;&c_m;&c_i;&c_ja;</trd>
  <trd lng="sv">kemi</trd>
</drv>

<drv mrk="hxemi.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc>
    <dif>
      <tld lit="&Hcirc;"/>a produkto.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_ib;&c_m;&c_ib;&c_k;&c_a;&c_t;</trd>,
    <trd>&c_h;&c_ib;&c_m;&c_ib;&c_k;&c_a;&c_l;&c_ib;&c_ib;</trd>,
    <trd>&c_h;&c_ib;&c_m;&c_ib;&c_ch;&c_n;&c_a;&c_je; &c_r;&c_e;&c_ch;&c_y;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Chemikalie</trd>
  <trd lng="en">chemical</trd>
  <trd lng="fr">substance chimique</trd>
  <trdgrp lng="nl">
    <trd>chemieproduct</trd>,
    <trd>chemicali&euml;n<klr> (= &hcirc;emia&jcirc;oj)</klr></trd>
  </trdgrp>
  <trd lng="pt">produto qu&iacute;mico</trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_i;&c_m;&c_i;&c_k;&c_a;&c_l;&c_i;&c_j;</trd>,
    <trd>&c_h;&c_i;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; &c_v;&c_je;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="sv">kemikalie</trd>
</drv>
</art>
