<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>front</rad>/o
</kap>
<drv mrk="front.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Anta&ubreve;a, la&ubreve;lar&gcirc;a, kvaza&ubreve;frunta parto de objekto:
      <ekz><tld/>o de konstrua&jcirc;o, de palaco, de armeo;</ekz>
      <ekz><tld/>a fenestro;</ekz>
      <ekz>
        <tld/>artikolo de gazeto
        <klr>(tiu &ccirc;e la plej anta&ubreve;a loko; &ccirc;efartikolo.)</klr>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="frunt.0o">frunto</ref>,
      <ref cel="fronto.0o">frontono</ref>,
      <ref cel="fasad.0o">fasado</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Front</trd>,
     <trd>Vorderseite</trd>,
     <trd>Stirnseite</trd></trdgrp>
  <trdgrp lng="ru">
    <trd>&c_f;&c_r;&c_o;&c_n;&c_t;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_d;&c_n;&c_ja;&c_ja; &c_s;&c_t;&c_o;&c_r;&c_o;&c_n;&c_a;</trd>
  </trdgrp>
</drv>
<drv mrk="front.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Sur la <tld/>a flanko. 
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>an der Front</trd>,
     <trd>an der Vorderseite</trd>,
     <trd>stirnseitig</trd></trdgrp>
</drv>
<drv mrk="front.0eal">
  <kap><tld/>e al</kap>
  <snc>
    <dif>
      Kontra&ubreve;rigarde al, vidalvide al. 
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>frontal</trd>,
     <trd>frontseitig</trd>,
     <trd>stirnseitig</trd></trdgrp>
  <trd lng="ru">&c_l;&c_i;&c_c;&c_o;&c_m; &c_k;</trd>
</drv>
<drv mrk="front.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Sin turni prezentante <tld/>on kontra&ubreve; io a&ubreve; iu por batali:
      <ekz><tld/>i kontra&ubreve; malamikon, dan&gcirc;eron, malbon&scirc;ancon;</ekz>
      <ekz>nia domo <tld/>as kontra&ubreve; (rigardas al) parko.</ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Front machen (gegen)</trd>,
     <trd>die <ind>Stirn bieten</ind></trd></trdgrp>
</drv>
<drv mrk="front.al0i">
  <kap>al<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="F"/>i kontra&ubreve;: <ekz>al<tld/>i dan&gcirc;eron, malsanon.</ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>gegen&uuml;berstehen</trd>,
     <trd>konfrontiert sein (mit)</trd></trdgrp>
</drv>
</art>
