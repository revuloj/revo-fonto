        <fnt>
          <bib>ChC</bib>
          <lok>4</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i al polica enketisto
        <fnt>
          <bib>ChB</bib>
          <lok>14</lok>
        </fnt>;
