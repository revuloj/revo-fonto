<drv mrk="front.0e_al">
  <kap><tld/>e al</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="front.0e_al.kie">
    <dif>
      <ref tip="super" cel="kontraux.0.kie">Kontra&ubreve;</ref>, kun
      sia <tld/>o rigardanta al:
      <ekz>
        lia kolego ne sentis sin tute hejme <tld/>e al la policano
        <fnt>
          <bib>ChB</bib>
          <lok>6</lok>
        </fnt>;
      </ekz>
      <ekz>
        nekapabla subpremi ridon <tld/>e al la komika viza&gcirc;o
        <fnt>
          <bib>ChC</bib>
          <lok>20</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e al mi, sur la hamaka deklivo, mi apogis la kajeran
        taglibron
        <fnt><aut>W. Schad</aut>
         <vrk><url ref="&Monato;/2000/007145.html">Orinoko-delto: Indianoj,
        mar&ccirc;oj, arbarego</url></vrk>
         <bib>Monato</bib>
        <lok>2000/11, p. 20</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> faris stre&ccirc;ajn ceremoniajn gestojn al la duoncirklo <tld/>e al si
        <fnt>
          <bib>Sol</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>frontal</trd>,
      <trd>frontseitig</trd>,
      <trd>stirnseitig</trd>
    </trdgrp>
    <trd lng="en">facing</trd>
    <trd lng="fr">en face de</trd>
    <trdgrp lng="hu">
      <trd>szemben</trd>,
      <trd>&aacute;tellenben</trd>,
      <trd>vizav&iacute;</trd>
    </trdgrp>
    <trd lng="nl">tegenover</trd>
    <trdgrp lng="pl">
      <trd>naprzeciwko</trd>,
      <trd>twarz&aogonek; <klr>do</klr></trd>,
      <trd>frontem <klr>do</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_l;&c_i;&c_c;&c_o;&c_m; &c_k;</trd>
  </snc>
  <snc mrk="front.0e_al.rilate">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="kontraux.0.al">Kontra&ubreve;</ref>,
      reagante al la &ccirc;eesto de:
      <ekz>
        mia febla percepto <tld/>e al la minacita mortkondamno
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 14</lok>
        </fnt>.
      </ekz>
      <ekz>
        la konduto de la homo de Neandertalo <tld/>e al la morto
        <fnt><aut>R. Roze</aut>, <vrk>Vivisekcio de polenero</vrk>,
        <bib>Monato</bib>
         <lok>1997/11, p.
        12</lok></fnt>;
      </ekz>
      <ekz>
        objektiva informado <tld/>e al la vipura &jcirc;urnalismo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Monato;/2000/007264.php">Morti
          miskomprene (1)</url></vrk>
          <lok>2000/12, p. 4</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e al tiaj fanatikaj frapfrazoj kaj tuta amaso da misinformoj,
        oni rajtas demandi sin, kiu &ccirc;i-rilate vere stultumas
        <fnt>

          <vrk><url ref="&Monato;/2000/007143.php">&Ccirc;u ni mortu
            miskomprene? (2)</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 23</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="konfront.0i">konfronti</ref>
    <trdgrp lng="pl">
      <trd>naprzeciwko</trd>,
      <trd>przeciw</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_p;&c_r;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_u;&c_p;&c_r;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_t;&c_v;&c_a;&c_r;&c_a;&c_m; <klr>(&c_d;&c_a; &c_ch;&c_a;&c_g;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  </trdgrp>
  <trd lng="cs">&ccaron;elem k</trd>
  <trd lng="fr">face &agrave;</trd>
  <trd lng="ja">&#xFF5E;<ind>&#x306B;&#x9762;&#x3057;&#x3066;  </ind><pr>&#x306B;&#x3081;&#x3093;&#x3057;&#x3066;</pr></trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_d;
  &c_l;&c_i;&c_c;&c_o;&c_m;<klr tip="amb">
  (&c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>
  <trd lng="sk">&ccaron;elom k</trd>
</drv>
