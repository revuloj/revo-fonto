    <trd lng="en">pia mater</trd>
  </snc>
  <trd lng="de">Pia mater</trd>
  <trd lng="fr">pie-m&egrave;re</trd>
  <trdgrp lng="hu">
    <trd>l&aacute;gy <ind>agyh&aacute;rtya</ind></trd>,
    <trd>pia mater</trd>
  </trdgrp>
  <trd lng="nl">zacht <ind>hersenvlies</ind></trd>
  <trd lng="ru">&c_m;&c_ja;&c_g;&c_k;&c_a;&c_ja; &c_m;&c_o;&c_z;&c_g;&c_o;&c_v;&c_a;&c_ja; &c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a;</trd>
</drv>
