<vortaro>
<art mrk="$Id: gonad.xml,v 1.5 2023/01/10 09:29:21 revo Exp $">
<kap>
  <rad>gonad</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="gonad.0o">
  <kap><tld/>o</kap>
  <snc mrk="gonad.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      <ref tip="super" cel="seks.0organo">Seksorgano</ref>, kiu
      produktas <ref tip="vid" cel="gamet.0o.GNT">gametojn</ref> kaj la
      seksajn <ref tip="vid" cel="hormon.0o.FIZL">hormonojn</ref>; <ref
      tip="sub" cel="testik.0o.ANA">testiko</ref> a&ubreve; <ref tip="sub"
      cel="ovari.0o.ANA">ovario</ref>:
      <ekz>
        la ina <tld/>o estas la ovujo, faklingve ovario, la vira <tld/>o
        nomi&gcirc;as testiko
        <fnt>
          <aut>C. St&oslash;p-Bowitz, M. B&eacute;la</aut>,
          <vrk><url
          ref="http://www.fw.hu/eventoj/steb/biologio/enkond-fokuso/f874-p08.htm">
          Enkonduko en la biologion</url></vrk>,
          <lok>Fokuso, 1987-04</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_n;&c_a;&c_d;&c_a;</trd>
  <trd lng="ca">g&ograve;nada</trd>
  <trd lng="cs">gon&aacute;da</trd>
  <trd lng="cy">gonad</trd>
  <trd lng="da">gonade</trd>
  <trd lng="de">Gonade</trd>
  <trd lng="el">&gamma;&omikron;&ny;&alfa_ton;&delta;&alfa;</trd>
  <trd lng="en">gonad</trd>
  <trd lng="es">g&oacute;nada</trd>
  <trd lng="et">sugun&auml;&auml;re</trd>
  <trd lng="eu">gonada</trd>
  <trd lng="fi">sukupuolirauhanen</trd>
  <trd lng="fr">gonade</trd>
  <trd lng="ga">g&oacute;nad</trd>
  <trd lng="gl">g&oacute;nada</trd>
  <trd lng="hy">&#1379;&#1400;&#1398;&#1377;&#1380;</trd>
  <trd lng="id">gonad</trd>
  <trd lng="io">gonado</trd>
  <trd lng="is">kynkirtill</trd>
  <trd lng="it">gonade</trd>
  <trd lng="jbo">ganti</trd>
  <trd lng="kk">&c_g;&c_o;&c_n;&c_a;&c_d;&c_t;&c_a;&c_r;</trd>
  <trd lng="ko">&#49373;&#49885;&#49368;</trd>
  <trd lng="nl">gonade</trd>
  <trd lng="pl">gonada</trd>
  <trdgrp lng="pt">
    <trd>g&oacute;nada</trd>,
    <trd>g&ocirc;nada</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_o;&c_n;&c_a;&c_d;&c_a;</trd>
  <trd lng="sv">gonad</trd>
  <trd lng="tr">gonad</trd>
  <trd lng="uk">&c_g;&c_o;&c_n;&c_a;&c_d;&c_i;</trd>
</drv>
</art>

