<art mrk="$Id: sabeli.xml,v 1.20 2024/02/21 09:23:32 revo Exp $">
<kap>
  <rad>sabelik</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="sabeli.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="brasik.krispa_0o">Krispa brasiko</ref>:
      <ekz>
        krispa brasiko a&ubreve; <tld/>o estas facila
        rikolta&jcirc;o kiu povas esti kultivata sur &ccirc;iuspecaj grundoj kaj estas tipa vintra legomo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=194207">Krispa brasiko</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj varioj de sabeliko estas kultivitaj kiel ornama&jcirc;oj
        <fnt>
          <vrk><url ref="http://esperantaretradio.blogspot.com/2020/09/sabeliko-escepta-sanefika-legomo.html"
              >Sabeliko - escepta sanefika legomo</url></vrk>
          <lok>Esperanta Retradio, 2020-09-16</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/20/Boerenkool.jpg/503px-Boerenkool.jpg" prm="CC-BY-SA-3.0"><tld/>o
        <fnt>
          <aut>Rasbak, propra verko</aut>
          <lok><url ref="&WCW;/File:Boerenkool.jpg">&WCW;/File:Boerenkool.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <snc mrk="sabeli.0o.savojbrasiko">
    <fnt><bib>JED</bib></fnt>
    <uzo tip="stl">ARK</uzo>
    <dif>
      Savoja <ref cel="brasik.0o">brasiko</ref>.
    </dif>
    <trd lng="de">Wirsing</trd>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_v;&c_o;&c_j;&c_s;&c_k;&c_a;&c_ja; &c_k;&c_a;&c_p;&c_u;&c_s;&c_t;&c_a;</trd>
  <trd lng="ca"><ind>col</ind> verda</trd>
  <trd lng="cs">kapusta kade&rcaron;av&aacute;</trd>
  <trd lng="de">Gr&uuml;nkohl</trd>
  <trdgrp lng="en">
    <trd>Savoy cabbage</trd>,
    <trd>kale</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>chou fris&eacute;</trd>,
    <trd>chou de Milan</trd>
  </trdgrp>
  <trd lng="hu">kelk&aacute;poszta</trd>
  <trd lng="ja">&#12481;&#12522;&#12513;&#12531;&#12461;&#12515;&#12505;&#12484;</trd>
  <trd lng="nl">savooienkool</trd>
  <trd lng="pl">jarmu&zdot;</trd>
  <trdgrp lng="pt">
   <trd>couve-de-sav&oacute;ia</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_a;&c_v;&c_o;&c_j;&c_s;&c_k;&c_a;&c_ja; <ind>&c_k;&c_a;&c_p;&c_u;&c_s;&c_t;&c_a;</ind></trd>
  <trd lng="sk">ku&ccaron;erav&aacute; kapusta</trd>
  <trd lng="uk">&c_s;&c_a;&c_v;&c_o;&c_j;&c_s;&c_mol;&c_k;&c_a; &c_k;&c_a;&c_p;&c_u;&c_s;&c_t;&c_a;</trd>
</drv>
</art>
