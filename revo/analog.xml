<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>7</ofc>
  <rad>analog</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="analog.0a">
  <kap><tld/>a</kap>
  <snc mrk="analog.0a.KOMUNE">
    <dif>
      <ref tip="vid" cel="simil.0a">Simila</ref>
      la&ubreve; kelkaj rilatoj al alia afero de malsama speco;
      parte simila.
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_n;&c_a;&c_l;&c_ja;&c_g;&c_ib;&c_ch;&c_n;&c_y;</trd>,
      <trd>&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
     <trd>entsprechend</trd>,
     <trd>gleichartig</trd>,
     <trd>analog</trd>
    </trdgrp>
    <trd lng="de">analog</trd>
    <trd lng="en">analogous</trd>
    <trd lng="fr">analogue</trd>
    <trd lng="es">an&aacute;logo</trd>
    <trd lng="nl">analoog</trd>
    <trdgrp lng="it">
      <trd>analogo</trd>,
      <trd>simile</trd>
    </trdgrp>
    <trd lng="ru">&c_a;&c_n;&c_a;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
    <trdgrp lng="sk">
      <trd>obdobn&yacute;</trd>,
      <trd>analogick&yacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="analog.0a.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      Simila la&ubreve; funkcio sed la&ubreve; biologia historio ne samdevena:
      <ekz>
        la flugiloj de vesperto estas organo <tld/>a, sed ne simila,
        je la flugiloj de birdo.
      </ekz>
    </dif>
    <trd lng="fr">analogue<klr> (biol.)</klr></trd>
  </snc>
  <snc mrk="analog.0a.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Funkcianta surbaze de fizika simulado, kaj precipe per kontinuaj
      grandoj:
      <ekz>
        <tld/>a komputilo, <tld/>a televido, <tld/>a modelo.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="cifer.0eca">cifereca</ref>
    </refgrp>
    <refgrp tip="sin">
      <ref cel="kontin3.0a">kontinua</ref>
    </refgrp>
    <trd lng="be">&c_a;&c_n;&c_a;&c_l;&c_ja;&c_g;&c_a;&c_v;&c_y;</trd>
    <trd lng="en">analog</trd>
    <trd lng="es">anal&oacute;gico</trd>
    <trd lng="fr">analogique</trd>
    <trd lng="nl">analoog</trd>
    <trd lng="it">analogico</trd>
    <trd lng="ru">&c_a;&c_n;&c_a;&c_l;&c_o;&c_g;&c_o;&c_v;&c_y;&c_j;</trd>
    <trd lng="sk">anal&oacute;gov&yacute;</trd>
  </snc>
</drv>

<drv mrk="analog.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Parta simileco de malsamspecaj objektoj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_n;&c_a;&c_l;&c_ja;&c_g;&c_ib;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_n;&c_a;&c_l;&c_ja;&c_g;&c_a;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Entsprechung</trd>,
     <trd>Analogie</trd>
  </trdgrp>
    <trd lng="en">analogousness</trd>
  <trdgrp lng="fr">
    <trd>analogie</trd>,
    <trd>similitude</trd>
  </trdgrp>
  <trd lng="nl">analogie</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_a;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>;
    <trd>&c_a;&c_n;&c_a;&c_l;&c_o;&c_g;&c_o;&c_v;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
