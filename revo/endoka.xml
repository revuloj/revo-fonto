<art mrk="$Id: endoka.xml,v 1.20 2025/12/03 13:06:35 revo Exp $">
<kap>
  <rad>endokard</rad>/o
<fnt><bib>PVEF</bib></fnt></kap>
<drv mrk="endoka.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Interna <ref tip="super" cel="membra.0o.ANA">membrano</ref> de la <ref tip="malprt"
      cel="kor.0o">koro</ref>:
      <ekz>
       endokardito estas inflamo de <tld/>o.
      </ekz>
    </dif>
    <ref tip="vid" cel="endoka2.0o">endokardito</ref>
  </snc>
  <trd lng="be">&c_e;&c_n;&c_d;&c_a;&c_k;&c_a;&c_r;&c_d;</trd>
  <trd lng="bg">&c_je;&c_n;&c_d;&c_o;&c_k;&c_a;&c_r;&c_d;&c_a;</trd>
  <trd lng="cs">vnit&rcaron;n&iacute; bl&aacute;na srde&ccaron;n&iacute;</trd>
  <trdgrp lng="de">
    <trd>Endokard<klr> [fak]</klr></trd>,
    <trd>Herzinnenhaut</trd>
  </trdgrp>
  <trd lng="en">endocardium</trd>
  <trd lng="fr">endocarde</trd>
  <trd lng="it">endocardio</trd>
  <trd lng="ja">&#24515;&#20869;&#33180;  <pr>&#12371;&#12371;&#12429;&#12394;&#12356;&#12414;&#12367;</pr></trd>
  <trd lng="nl">endocardium</trd>
  <trd lng="pl">wsierdzie</trd>
  <trd lng="pt">endoc&aacute;rdio</trd>
  <trd lng="ru">&c_e;&c_n;&c_d;&c_o;&c_k;&c_a;&c_r;&c_d;</trd>
  <trd lng="sk">vn&uacute;torn&aacute; blana srde&ccaron;n&aacute;</trd>
</drv>
</art>
