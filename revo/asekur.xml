<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>asekur</rad>/i
</kap>
<drv mrk="asekur.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Protekti per kontrakto kun kompanio, kiu en okazo de doma&gcirc;o a&ubreve; perdo
      pagu kompensan monon:
      <ekz>
        <tld/>i domon kontra&ubreve; incendio;
      </ekz>
      <ekz>
        sin <tld/>i kontra&ubreve; akcidentoj, morto;
      </ekz>
      <ekz>
        <tld/>i sian vivon.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Protekti, sekurigi kontra&ubreve; io:
      <ekz>
        lia sorto estas de nun <tld/>ita;
      </ekz>
      <ekz>
        laboro <tld/>as kontra&ubreve; mizero.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="cert.0igi">certigi</ref>,
      <ref cel="gard.0i">gardi</ref>,
      <ref cel="dangxe">sendan&gcirc;erigi</ref>,
      <ref cel="sxirm.0i">&scirc;irmi</ref>,
      <ref cel="ten.sub0i">subteni</ref>.
    </refgrp>
  </snc>
  <trd lng="en">insure</trd>
  <trd lng="fr">assurer</trd>
  <trd lng="nl">verzekeren</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_a;&c_h;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">f&ouml;rs&auml;kra</trd>
</drv>

<drv mrk="asekur.0ado">
  <kap><tld/>(ad)o</kap>
  <snc>
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <trd lng="fr">assurance</trd>
  <trd lng="nl">verzekering</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_a;&c_h;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="asekur.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Kontrakto por <tld/>i.
    </dif>
  </snc>
  <trd lng="fr">assurance</trd>
  <trd lng="nl">verzekering</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_a;&c_h;&c_o;&c_v;&c_k;&c_a;</trd>
  <trd lng="sv">f&ouml;rs&auml;kring</trd>
</drv>

<drv mrk="asekur.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <ekz>
        <tld/>a kompanio, agento, kontrakto.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">d'<ind>assurance</ind></trd>
  <trd lng="nl">verzeker-</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_a;&c_h;&c_o;&c_v;&c_o;&c_j;</trd>
</drv>

<drv mrk="asekur.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Tiu, kiu sin okupas pri <tld/>oj.
    </dif>
  </snc>
  <trd lng="fr">assureur</trd>
  <trd lng="nl">verzekeraar</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_a;&c_h;&c_o;&c_v;&c_shch;&c_i;&c_k;</trd>
</drv>
</art>
