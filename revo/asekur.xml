</kap>
<drv mrk="asekur.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="asekur.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="protek.0i">Protekti</ref> per kontrakto kun kompanio, 
      kiu en okazo de doma&gcirc;o a&ubreve; perdo pagu kompensan monon:
      <ekz>
        <tld/>i domon kontra&ubreve; incendio;
      </ekz>
      <ekz>
        sin <tld/>i kontra&ubreve; akcidentoj, morto;
      </ekz>
      <ekz>
        &scirc;i prenis sian &scirc;paritan monon por asekuri siajn du infanojn, tiel ke en okazo de
        bezono la <tld/>o povos financi lernadon de profesio a&ubreve; studado
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2008/009448.php">Krimaj ri&ccirc;uloj</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        Munkena Reasekuro <klr>[&#x2026;]</klr> <tld/>as <tld/>kompaniojn kontra&ubreve; plej grandaj
        riskoj speciale pro naturkatastrofoj
        <fnt>
          <bib>Monato</bib>
          <aut>Werner Fu&szlig;</aut>
          <vrk><url ref="&Monato;/2011/010273.php">Verdigu la dezertojn!</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">versichern</trd>
    <trd lng="sk">poisti&tcaron;</trd>
  </snc>
  <snc mrk="asekur.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Protekti, <ref tip="super" cel="sekur.0igi">sekurigi</ref> kontra&ubreve; io:
      <ekz>
        lia sorto estas de nun <tld/>ita;
      </ekz>
      <ekz>
        laboro <klr>[&#x2026;]</klr> <tld/>as kontra&ubreve; mizero
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="cert.0igi">certigi</ref>,
      <ref cel="gard.0i">gardi</ref>,
      <ref cel="dangxe">sendan&gcirc;erigi</ref>,
      <ref cel="ten.sub0i">subteni</ref>,
      <ref cel="sxirm.0i">&scirc;irmi</ref>
    </refgrp>
    <trd lng="de">sichern</trd>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_r;&c_a;&c_h;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>pojistit <klr>(se)</klr> (proti n&ecaron;&ccaron;emu</trd>,
    <trd>p&rcaron;ed n&ecaron;&ccaron;&iacute;m)</trd>
  </trdgrp>
  <trd lng="en">insure</trd>
  <trd lng="es">asegurar</trd>
  <trd lng="fr">assurer</trd>
  <trd lng="he">&lamed;&bet;&tet;&het;</trd>
  <trd lng="hu">biztos&iacute;t<klr tip="amb"> (vmi ellen)</klr></trd>
  <trd lng="it">assicurare<klr> (econ. e fig.)</klr></trd>
  <trd lng="ja">&#x4FDD;&#x967A;&#x3092;&#x304B;&#x3051;&#x308B;  <pr>&#x307B;&#x3051;&#x3093;&#x3092;&#x304B;&#x3051;&#x308B;</pr></trd>
  <trd lng="nl">verzekeren</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_a;&c_h;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">f&ouml;rs&auml;kra</trd>
  <trd lng="uk">&c_s;&c_t;&c_r;&c_a;&c_h;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="asekur.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="asekur.0ado.EKON">
  <uzo tip="fak">EKON</uzo>
    <dif>
      Ago <tld/>i:
      <ekz>
        <tld/>ado kontra&ubreve; kurzo&scirc;an&gcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Bo Sandelin</aut>
          <vrk><url ref="&Monato;/2004/008235.html">Svedoj balotis kontra&ubreve; e&ubreve;ro</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_r;&c_a;&c_h;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>poji&scaron;&tcaron;ov&aacute;n&iacute;</trd>,
    <trd>zabezpe&ccaron;ov&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Versichern</trd>
  <trd lng="fr">assurance</trd>
  <trd lng="he">&bet;&yod;&tet;&vav;&het;</trd>
  <trd lng="hu">biztos&iacute;t&aacute;s</trd>
  <trd lng="it">assicurare<klr> (econ.)</klr></trd>
  <trd lng="nl">verzekering</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_a;&c_h;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">pois&tcaron;ovanie</trd>
</drv>

<drv mrk="asekur.0o">
  <kap><tld/>o</kap>
  <snc mrk="asekur.0o.EKON">
  <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="kontra3.0o">Kontrakto</ref> por <tld/>i:
      <ekz>
        pri la sorto de mia familio post mia morto mi ne havos zorgojn, &ccirc;ar krom la
        pensimono, anka&ubreve; la abunda <ind>viv<tld/>o</ind> donos sufi&ccirc;an fundamenton
        <fnt>
          <bib>VaK</bib>
          <lok>Unua &Ccirc;apitro</lok>
        </fnt>;
        <trd lng="de">Lebensversicherung</trd>
      </ekz>
      <ekz>
        mi trovis dokumentojn de la kontra&ubreve;malsana <tld/>o
        <fnt>
          <bib>ChC</bib>
          <lok>2</lok>
        </fnt>;
      </ekz>
      <ekz>
        47 milionoj homoj vivas sen <ind>san<tld/>o</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2009/009916.php">Fendita nacio</url></vrk>
          <lok>2009</lok>
        </fnt>;
        <trd lng="de">Krankenversicherung</trd>
      </ekz>
      <ekz>
        plialtigi impostojn por <ind>socia <tld/>o</ind>
        <fnt>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2000/006806.php">&Ccirc;iea &scirc;parado</url></vrk>
          <bib>Monato</bib>
          <lok>2000/05, p. 7</lok>
        </fnt>;
        <trd lng="de">Sozialversicherung</trd>
      </ekz>
      <ekz>
        dum 5 jaroj li ricevis subvencion el la <ind>senlabor-<tld/>o</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2008/009448.php">Krimaj ri&ccirc;uloj</url></vrk>
          <lok>2008</lok>
        </fnt>;
        <trd lng="de">Arbeitslosenversicherung</trd>
      </ekz>
      <ekz>
        reformi la le&gcirc;an <ind>pensi-<tld/>o</ind>n
        <fnt>
       <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2000/006835.php">Pekoj de patriarko</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 9</lok>
        </fnt>.
        <trd lng="de">Rentenversicherung</trd>
      </ekz>
    </dif>
  </snc>
  <snc mrk="asekur.0o.FIG">
    <dif>
      Sekurigo, protekto kontra&ubreve; io:
      <ekz>
        <klr>[&scirc;i]</klr> kun &gcirc;ojo enmetis sian manon en la manon de neri&ccirc;a, sed laborema
        homo, pensante, ke <klr>[&#x2026;tiel]</klr> &scirc;ia estonteco ricevis sufi&ccirc;an <tld/>on
        kontra&ubreve; la suferoj de soleco
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_r;&c_a;&c_h;&c_o;&c_w;&c_k;&c_a;</trd>
  <trd lng="bg">&c_z;&c_a;&c_s;&c_t;&c_r;&c_a;&c_h;&c_o;&c_v;&c_a;&c_n;&c_je;</trd>
  <trd lng="bo">&t_nya;&t_drengbu;&t_na;&t_tsheg;&t_sa;&t_rata;&t_shabchu;&t_nga;&t_tsheg;&t_-a;&t_ga;&t_na;&t_tsheg;&t_la;&t_drengbu;&t_na;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>asekurace</trd>,
    <trd>poj. smlouva</trd>,
    <trd>poji&scaron;t&ecaron;n&iacute;</trd>,
    <trd>zabezpe&ccaron;en&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Versicherung</trd>,
    <trd>Absicherung</trd>
  </trdgrp>
  <trd lng="en">insurance</trd>
  <trd lng="es">seguro</trd>
  <trd lng="fr">assurance</trd>
  <trd lng="he">&bet;&yod;&tet;&vav;&het;</trd>
  <trd lng="hu">biztos&iacute;t&aacute;s<klr tip="amb"> (szerz&odblac;d&eacute;s)</klr></trd>
  <trd lng="it"><klr>(contratto d')</klr>assicurazione</trd>
  <trd lng="ja">&#x4FDD;&#x967A;  <pr>&#x307B;&#x3051;&#x3093;</pr></trd>
  <trd lng="nl">verzekering</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_a;&c_h;&c_o;&c_v;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>poistenie</trd>,
    <trd>poistka</trd>
  </trdgrp>
  <trd lng="sv">f&ouml;rs&auml;kring</trd>
  <trdgrp lng="uk">
    <trd>&c_a;&c_s;&c_je;&c_k;&c_u;&c_r;&c_a;&c_c;&c_ib;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_h;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_h;&c_o;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4FDD;&#x9669;&#x5546; <pr>b&#x1CE;oxi&#x1CE;nsh&#x101;ng</pr></trd>,
    <trd>&#x4FDD;&#x96AA;&#x5546; <pr>b&#x1CE;oxi&#x1CE;nsh&#x101;ng</pr></trd>,
    <trd>&#x4FDD;&#x9669;&#x4E1A;&#x8005; <pr>b&#x1CE;oxi&#x1CE;ny&#xE8;zh&#x11B;</pr></trd>,
    <trd>&#x4FDD;&#x96AA;&#x696D;&#x8005; <pr>b&#x1CE;oxi&#x1CE;ny&#xE8;zh&#x11B;</pr></trd>,
    <trd>&#x4FDD;&#x9669;&#x673A;&#x6784; <pr>b&#x1CE;oxi&#x1CE;nj&#x12B;g&#xF2;u</pr></trd>,
    <trd>&#x4FDD;&#x96AA;&#x6A5F;&#x69CB; <pr>b&#x1CE;oxi&#x1CE;nj&#x12B;g&#xF2;u</pr></trd>
  </trdgrp>
</drv>

<drv mrk="asekur.0a">
  <kap><tld/>a</kap>
  <snc mrk="asekur.0a.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        direktoro de <ind><tld/>a kompanio</ind>
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 8-9 (58-59)</lok>
        </fnt>;
        <trd lng="de">Versicherungsgesellschaft</trd>
      </ekz>
      <ekz>
        mi estas <ind><tld/>a agento</ind> kaj mia patro estas policisto
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 11 (109)</lok>
        </fnt>;
        <trd lng="de">Versicherungsvertreter</trd>
      </ekz>
      <ekz>
        <tld/>a statistiko
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2013/010943.php">Kiel ku&scirc;i klinike?</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        nek la &scirc;tato nek la <tld/>aj kompanioj havas sufi&ccirc;e da mono por pagi tiom da
        pensioj
        <fnt>
          <aut>Walter Klag</aut>
            <vrk><url ref="&Monato;/2000/007101.php">Pensoj pensiaj</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 12</lok>
          </fnt>;
      </ekz>
      <ekz>
        ili apena&ubreve; ricevos <tld/>ajn a&ubreve; registarajn kompensojn
        <fnt>
	<aut>Guido van Damme</aut>
	<vrk><url ref="&Monato;/2001/007272.php">Reen al la edena paradizo</url></vrk>
	<bib>Monato</bib>
	<lok>2001/02, p. 20</lok>
        </fnt>;
      </ekz>
      <ekz>
        montru al mi vian <tld/>an kontrakton por la a&ubreve;to
        <fnt>
          <bib>Monato</bib>
          <aut>Sergeo Dereviannykh</aut>
          <vrk><url ref="&Monato;/2004/007562.html">Kiu instigas nin al farado, kiel plej multe faras</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a sumo de la tuto estis 70 milionoj da e&ubreve;roj
        <fnt>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2000/007065.php">VIPE 2000, a&ubreve; &ccirc;u la po&scirc;tmarkoj
          malaperos?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/09, p. 18</lok>
          </fnt>.
      </ekz>

    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_r;&c_a;&c_h;&c_a;&c_v;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>asekura&ccaron;n&iacute;</trd>,
    <trd>pojistn&yacute;</trd>,
    <trd>poji&scaron;&tcaron;ovac&iacute;</trd>
  </trdgrp>
  <trd lng="de">Versicherungs-</trd>
  <trd lng="fr">d'<ind>assurance</ind></trd>
  <trd lng="he">&bet;&yod;&tet;&vav;&het;&yod;</trd>
  <trdgrp lng="hu">
    <trd>biztos&iacute;t&oacute;-</trd>,
    <trd>biztos&iacute;t&aacute;si</trd>
  </trdgrp>
  <trd lng="it">assicurativo</trd>
  <trd lng="nl">verzeker-</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_a;&c_h;&c_o;&c_v;&c_o;&c_j;</trd>
  <trd lng="sk">poistn&yacute;</trd>
  <trd lng="uk">&c_s;&c_t;&c_r;&c_a;&c_h;&c_o;&c_v;&c_i;&c_j;</trd>
</drv>

