    <trd lng="fr">ensemble<klr tip="ind"> (math.)</klr></trd>
    <trd lng="it">insieme<klr tip="ind"> (mat.)</klr></trd>
    <trd lng="ru">&c_m;&c_n;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_o;<klr tip="ind">
    (&c_m;&c_a;&c_t;.)</klr></trd>
