        <trd>groupe</trd>,
        <trd>jeu<klr tip="ind"> (ensemble d'objets)</klr></trd>
      </trdgrp>
      <trdgrp lng="it">
        <trd>insieme</trd>,
        <trd>gruppo</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>zbi&oacute;r</trd>,
        <trd>grupa</trd>
