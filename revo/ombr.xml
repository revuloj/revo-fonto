<drv mrk="ombr.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>

  <snc mrk="ombr.0umi.malheligi">
    <dif>
      Malheligi, obtuzigi:
      <ekz>
        somero de l' amo min premas sur vojo, sen via <tld/>umo mi
        mortus sen &gcirc;ojo
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga
          Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">MUZ</uzo>
        kvankam Mercury la&ubreve;nature estis tenoro, li scipovis multmaniere
        <tld/>umi la diversajn registrojn de sia 3&#xBD;-oktavo-vo&ccirc;o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=141581">Falseto</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>

  <snc mrk="ombr.0umi.BELA">
