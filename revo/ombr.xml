    <trd>&#x5F71; <pr>y&#x1D0;ng</pr></trd>,
    <trd>&#38452;&#x5F71; <pr>y&#x12B;ny&#x1D0;ng</pr></trd>,
    <trd>&#38512;&#x5F71; <pr>y&#x12B;ny&#x1D0;ng</pr></trd>,
    <trd>&#x5F71;&#x5B50; <pr>y&#x1D0;ngzi</pr></trd>,
    <trd>&#x9ED1;&#x5F71; <pr>h&emacron;iy&#x1D0;ng</pr></trd>,
    <trd>&#26263;&#x5F71; <pr>&#xE0;ny&#x1D0;ng</pr></trd>,
    <trd>&#x836B; <pr>y&#xEC;n</pr></trd>,
    <trd>&#x852D; <pr>y&#xEC;n</pr></trd>,
    <trd>&#x6A3E; <pr>yu&#xE8;</pr></trd>,
    <trd>&#26641;&#x836B; <pr>sh&#xF9;y&#xEC;n</pr></trd>,
    <trd>&#x6A39;&#x852D; <pr>sh&#xF9;y&#xEC;n</pr></trd>,
    <trd>&#x5F71;&#x513F; <pr>y&#x1D0;ngr</pr></trd>,
    <trd>&#x5F71;&#20818; <pr>y&#x1D0;ngr</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ombr.0a">
  <kap><tld/>a,
    <var><kap><tld/>oplena</kap></var>,
    <var><kap><tld/>ori&ccirc;a</kap></var>
  </kap>
  <snc mrk="ombr.0a.KOMUNE">
    <dif>
      Estanta en <tld/>o:
      <ekz>
        <tld/>a valo;
      </ekz>
      <ekz>
        <tld/>a cipreso, kverko, monto, flanko
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a aleo de maljunaj arboj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        li eniris en la <tld/>oplenan &gcirc;ardenon kaj malaperis inter la
        arboj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ori&ccirc;aj arboj &scirc;irmas &gcirc;in per sia <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 40:22</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_je;&c_n;&c_ja;&c_v;&c_y;</trd>,
    <trd>&c_c;&c_ja;&c_n;&c_ib;&c_s;&c_t;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>disheol<klr tip="ind"> (ag.)</klr></trd>,
    <trd>disheoliet</trd>
  </trdgrp>
  <trd lng="de">schattig</trd>
  <trdgrp lng="fr">
    <trd>ombrag&eacute;</trd>,
    <trd>ombreux</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&aacute;rny&eacute;kos</trd>,
    <trd>&aacute;rnyas</trd>
  </trdgrp>
  <trd lng="id">teduh</trd>
  <trdgrp lng="ja">
    <trd>&#x5F71;&#x306E;&#12354;&#x308B;  <pr>&#x304B;&#12370;&#x306E;&#12354;&#x308B;</pr></trd>,
    <trd>&#x65E5;&#x306E;&#x5F53;&#x305F;&#12425;&#x306A;&#12356; 
    <pr>&#x306B;&#12385;&#x306E;&#12354;&#x305F;&#12425;&#x306A;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">schaduwrijk</trd>
  <trd lng="pl">cienisty</trd>
  <trd lng="ro">umbrit&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_je;&c_n;&c_je;&c_v;&c_o;&c_j;</trd>,
    <trd>&c_t;&c_je;&c_n;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">skuggig</trd>
  <trd lng="uk">&c_t;&c_ib;&#x43D;&c_i;&c_s;&c_t;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x836B; <pr>y&#xEC;n</pr></trd>,
    <trd>&#x852D; <pr>y&#xEC;n</pr></trd>,
    <trd>&#x7DA0;&#38512; <pr>l&#xF9;:y&#x12B;n</pr></trd>,
    <trd>&#x7EFF;&#38452; <pr>l&#xF9;:y&#x12B;n</pr></trd>,
    <trd>&#x906E;&#x836B; <pr>zh&emacron;y&#xEC;n</pr></trd>,
    <trd>&#x906E;&#x852D; <pr>zh&emacron;y&#xEC;n</pr></trd>,
    <trd>&#x906E;&#38452; <pr>zh&emacron;y&#x12B;n</pr></trd>,
    <trd>&#x906E;&#38512; <pr>zh&emacron;y&#x12B;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ombr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ombr.0i.KOMUNE">
    <dif>
      &Jcirc;eti <tld/>on sur ion:
      <ekz>
        kiu plantis anta&ubreve; la domo la belajn kreska&jcirc;ojn, kiuj tiel
        pentrinde &gcirc;in <tld/>as?
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        unu arbeta&jcirc;o de tiu &ccirc;i kreska&jcirc;o <tld/>as la spacon de tri
        kvadrataj metroj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la malgrandaj lipharoj <tld/>is la maldikajn lipojn
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        Atrejo <tld/>is sian viza&gcirc;on per la mano kaj ekvidis je
        proksimuma distanco de duono de mejlo neregule formitan
        &scirc;tonpordegon
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Geermitoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        frunto <tld/>ita de zorgoj.
      </ekz>
    </dif>
    <ref tip="vid" cel="nub.0o">nubo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_c;&c_ja;&c_n;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_ib;&c_d;&c_a;&c_c;&c_mol; <ind>&c_c;&c_je;&c_n;&c_mol;</ind></trd>
  </trdgrp>
  <trd lng="br">disheolia&ntilde;<klr tip="ind"> (v.k.)</klr></trd>
  <trdgrp lng="de">
     <trd>beschatten</trd>,
     <trd><ind>Schatten</ind> werfen</trd>
  </trdgrp>
  <trd lng="es">ensombrecer</trd>
  <trdgrp lng="fr">
    <trd>assombrir</trd>,
    <trd>ombrager</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&aacute;rny&eacute;kol</trd>,
    <trd>be&aacute;rny&eacute;kol</trd>
  </trdgrp>
  <trd lng="id">me<ind>naung</ind>i</trd>
  <trdgrp lng="id">
    <trd><baz>payung</baz>memayungi</trd>,
    <trd><baz>naung</baz>menaungi</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5F71;&#12434;&#x843D;&#12392;&#12377;  <pr>&#x304B;&#12370;&#12434;&#x304A;&#12392;&#12377;</pr></trd>,
    <trd>&#38512;&#12425;&#12377;  <pr>&#x304B;&#12370;&#12425;&#12377;</pr></trd>
  </trdgrp>
  <trd lng="nl">beschaduwen</trd>
  <trdgrp lng="pl">
    <trd>rzuca&cacute;  <ind>cie&nacute;</ind></trd>,
    <trd>zacienia&cacute;</trd>,
    <trd>zaciemnia&cacute;</trd>
  </trdgrp>
  <trd lng="pt">sombrear</trd>
  <trdgrp lng="ro">
    <trd>arunc&abreve; o  <ind>umbr&abreve;</ind></trd>,
    <trd>adumbri</trd>,
    <trd>&icirc;ntuneca</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_o;&c_s;&c_a;&c_t;&c_mol; <ind>&c_t;&c_je;&c_n;&c_mol;</ind></trd>,
    <trd>&c_z;&c_a;&c_t;&c_je;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">skugga</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_i;&c_d;&c_a;&c_t;&c_i; &c_t;&c_ib;&#x43D;&#x44C;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_ib;&#x43D;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_ib;&#x43D;&#x44F;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x836B; <pr>y&#xEC;n</pr></trd>,
    <trd>&#x852D; <pr>y&#xEC;n</pr></trd>,
    <trd>&#22312;&#38452;&#x5F71;&#x4E2D; <pr>z&#xE0;iy&#x12B;ny&#x1D0;ngzh&#x14D;ng</pr></trd>,
    <trd>&#22312;&#38512;&#x5F71;&#x4E2D; <pr>z&#xE0;iy&#x12B;ny&#x1D0;ngzh&#x14D;ng</pr></trd>,
    <trd>&#x7D27;&#x76EF; <pr>j&#x1D0;nd&#x12B;ng</pr></trd>,
    <trd>&#x7DCA;&#x76EF; <pr>j&#x1D0;nd&#x12B;ng</pr></trd>,
    <trd>&#x836B;&#x853D; <pr>y&#xEC;nb&#xEC;</pr></trd>,
    <trd>&#x852D;&#x853D; <pr>y&#xEC;nb&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ombr.0ajxo">
  <kap><tld/>a&jcirc;o,
  <var><kap>sub<tld/>o</kap></var></kap>
  <snc mrk="ombr.0ajxo.KOMUNE">
    <dif>
      Loko <tld/>ita:
      <ekz>
        promeni sub densaj <tld/>a&jcirc;oj;
      </ekz>
      <ekz>
        sa&gcirc;a dervi&scirc;o kiu pasis multe da tempo sub la <tld/>a&jcirc;o de loka
        arbo kaj poste estis entombigita apud &gcirc;i
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=383818">Aleksandropolo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_je;&c_n;&c_mol;</trd>,
    <trd>&c_c;&c_ja;&c_n;&c_jo;&c_k;</trd>,
    <trd>&c_c;&c_ja;&c_n;&c_ib;&c_s;&c_t;&c_a;&c_je; &c_m;&c_je;&c_s;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="br">
