<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ulul.xml,v 1.28 2023/10/21 16:50:27 revo Exp $">
<kap>
  <rad>ulul</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ulul.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ulul.0i.ZOO_strigo">
    <dif>
      <ref tip="super" cel="blek.0i.beste">Bleki</ref>
      kiel strigo a&ubreve; noktuo.
    </dif>
    <trd lng="be">&c_v;&c_u;&c_h;&c_k;&c_a;&c_c;&c_mol;</trd>
    <trd lng="cs">houkat</trd>
    <trd lng="en">hoot</trd>
    <trd lng="es">ulular</trd>
    <trdgrp lng="fa">
      <trd>&a_h;&a_w;&a_h;&a_w; &f_k;&a_r;&a_d;&a_n;</trd>,
      <trd>&a_j;&f_y;&a_gh; &f_k;&a_sh;&f_y;&a_d;&a_n;<klr tip="amb"> (&a_j;&a_gh;&a_d;)</klr></trd>
    </trdgrp>
    <trd lng="fr">hululer</trd>
    <trd lng="hu">huhog</trd>
    <trdgrp lng="it">
      <trd>gridare<klr> (di rapace notturno)</klr></trd>,
      <trd>ululare<klr> (di rapace notturno)</klr></trd>
    </trdgrp>
    <trd lng="nl">roepen</trd>
    <trd lng="ru">&c_u;&c_h;&c_a;&c_t;&c_mol;</trd>
    <trd lng="de">schreien <klr>(Eule)</klr></trd>
  </snc>
  <snc mrk="ulul.0i.ZOO_lupo">
    <dif>
      <ref tip="super" cel="blek.0i.beste">Bleki</ref>
      kiel sova&gcirc;a hundo a&ubreve; lupo:
      <ekz>
        timinda lupo, <tld/>ante venas
        <fnt>
       <aut>Manoel Borges dos Santos</aut>
          <vrk><url ref="&Monato;/2000/006863.php">Poemoj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/04, p. 32</lok>
        </fnt>!
      </ekz>
      <ekz>
        en la ka&gcirc;oj <tld/>as &scirc;akaloj
        <fnt>
          <aut>S. Bubeni&ccaron;</aut>,
          <vrk>Sub Cirkotendo</vrk>,
          <lok>1969</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_y;&c_c;&c_mol;</trd>
    <trd lng="cs">v&yacute;t</trd>
    <trd lng="de">heulen <klr>(Wolf)</klr></trd>
    <trd lng="en">howl</trd>
    <trd lng="es">aullar</trd>
    <trd lng="fa">&a_z;&a_w;&a_z;&a_h; &f_k;&a_sh;&f_y;&a_d;&a_n;</trd>
    <trdgrp lng="fi">
      <trd>ulvoa</trd>,
      <trd>ulista</trd>
    </trdgrp>
    <trd lng="fr">hurler<klr> (animal)</klr></trd>
    <trdgrp lng="hu">
      <trd>von&iacute;t</trd>,
      <trd>&uuml;v&ouml;lt</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>gridare<klr> (ululare)</klr></trd>,
      <trd>ululare</trd>
    </trdgrp>
    <trd lng="nl">huilen</trd>
    <trd lng="pt">uivar</trd>
    <trd lng="ru">&c_v;&c_y;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="ca">udolar</trd>
  <trdgrp lng="ja">
    <trd>&#x9060;&#x5420;&#x3048;&#x3092;&#x3059;&#x308B;  <pr>&#x3068;&#x304A;&#x307C;&#x3048;&#x3092;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x307B;&#x3046;&#x307B;&#x3046;&#x3068;&#x9CF4;&#x304F;  <pr>&#x307B;&#x3046;&#x307B;&#x3046;&#x3068;&#x306A;&#x304F;</pr></trd>,
    <trd>&#x3046;&#x306A;&#x308B;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>h&uacute;ka&tcaron;</trd>,
    <trd>zav&yacute;ja&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_t;&c_i; (&#x43F;&c_r;&#x43E; &c_v;&#x43E;&c_v;&#x43A;&c_a; &c_ib; &c_t;.&#x43F;.</trd>,
    <trd>&c_t;&c_zh; &#x43F;&c_r;&#x43E; &c_s;&c_i;&c_r;&c_je;&#x43D;&c_u;)</trd>,
    <trd>&#x43A;&c_r;&c_i;&c_ch;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_u;&c_h;&c_a;&c_t;&c_i; (&#x43F;&c_r;&#x43E; &c_s;&#x43E;&c_v;&c_u; &c_ib; &c_t;.&#x43F;.)</trd>
  </trdgrp>
</drv>
</art>
