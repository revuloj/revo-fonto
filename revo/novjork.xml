<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>Novjork</rad>o</kap>
<uzo tip="fak">GEOG</uzo>
<drv mrk="novjork.0o">
  <kap><tld/>o</kap>
  <fnt><bib>BEE</bib></fnt>
  <snc>
    <dif>
      Plej grava urbo de Usono (74&ring;00'W, 40&ring;43'N), trafluata
      de la rivero Hudsono. Sidloko de <ref tip="vid"
      cel="naci.Unuigxintaj_0oj">UN</ref> kaj de granda akciborso. Origina
      nomo
      estis Nov-Amsterdamo.
    </dif>
    <trdgrp lng="de">
      <trd>New York</trd>,
      <trd>Neu-York<klr> [ark]</klr></trd>
    </trdgrp>
    <trd lng="en">New York</trd>
    <trd lng="hu">New York</trd>
    <trd lng="nl">New York</trd>
  </snc>
</drv>

<drv mrk="novjork.0akciborso">
  <kap><tld/>a Akciborso</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Bilborso de Novjorko, dum la 20a jarcento la&ubreve; vendoj plej
      granda borso de la mondo.
    </dif>
    <trdgrp lng="de">
      <trd>New-Yorker Aktienb&ouml;rse</trd>,
      <trd>Aktienb&ouml;rse New York</trd>,
      <trd>Effektenb&ouml;rse New York</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>New York <ind>Stock Exchange</ind></trd>,
      <trd>NYSE</trd>,
      <trd>big board</trd>,
      <trd>Wall street</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>New York-i <ind>&Eacute;rt&eacute;kt&odblac;zsde</ind></trd>,
      <trd>NYSE</trd>,
      <trd>Wall street</trd>
    </trdgrp>
    <trd lng="nl">beurs van New York</trd>
  </snc>
</drv>
</art>
