<kap>
  <ofc>9</ofc><rad>akupunktur</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="akupunktur.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="akupunktur.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Kuraca metodo el la tradicia &ccirc;ina medicino, kiu konsistas en
      pikado de difinitaj korpopunktoj per pingloj.
      <!-- Kuraca metodo, kiu uzas pinglojn enpikatajn en la supra
      ha&ubreve;to
      &ccirc;e difinitaj lokoj de la korpo.
      -->
    </dif>
    <trdgrp lng="zh">
    <trd>&#26045;&#20197;&#37341;&#28792; <pr>sh&#299;y&#464;zh&emacron;nji&#468;</pr></trd>,
    <trd>&#26045;&#20197;&#38024;&#28792; <pr>sh&#299;y&#464;zh&emacron;nji&#468;</pr></trd>,
    <trd>&#37341;&#21050; <pr>zh&emacron;nc&#236;</pr></trd>,
    <trd>&#38024;&#21050; <pr>zh&emacron;nc&#236;</pr></trd>,
    <trd>&#37341;&#27835; <pr>zh&emacron;nzh&#236;</pr></trd>,
    <trd>&#38024;&#27835; <pr>zh&emacron;nzh&#236;</pr></trd>,
    <trd>&#37341;&#21050;&#30274;&#27861; <pr>zh&emacron;nc&#236;li&#225;of&#462;</pr></trd>,
    <trd>&#38024;&#21050;&#30103;&#27861; <pr>zh&emacron;nc&#236;li&#225;of&#462;</pr></trd>
  </trdgrp>
</snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Anka&#365; eblas diri <ke>nadlopikkuracado</ke>, <ke>nadlopikado</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_a;&c_k;&c_u;&c_p;&c_u;&c_n;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>,
    <trd>&c_ib;&c_g;&c_o;&c_l;&c_k;&c_a;&c_w;&c_k;&c_o;&c_l;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="bg">&c_a;&c_k;&c_u;&c_p;&c_u;&c_n;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>
  <trd lng="es">acupuntura</trd>
  <trd lng="de">Akupunktur</trd>
  <trd lng="en">acupuncture</trd>
  <trd lng="fa">&a_T;&a_b; &a_s;&a_w;&a_z;&a_n;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>acuponcture</trd>,
    <trd>acupuncture</trd>
  </trdgrp>
  <trd lng="he">&#1488;&#1463;&#1511;&#1493;&#1468;&#1508;&#1468;&#1493;&#1468;&#1504;&#1511;&#1496;&#1493;&#1468;&#1512;&#1464;&#1492;</trd>
  <trd lng="hu">akupunkt&uacute;ra</trd>
  <trdgrp lng="ja">
    <trd>&#37756;&#30274;&#27861;  <pr>&#12399;&#12426;&#12426;&#12423;&#12358;&#12411;&#12358;</pr></trd>,
    <trd>&#37756;&#27835;&#30274;  <pr>&#12399;&#12426;&#12385;&#12426;&#12423;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">acupunctuur</trd>
  <trd lng="pl">akupunktura</trd>
  <trdgrp lng="pt">
   <trd>acupuntura</trd>
  </trdgrp>
  <trd lng="ro">acupuntur&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_k;&c_u;&c_p;&c_u;&c_n;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>,
    <trd>&c_i;&c_g;&c_l;&c_o;&c_u;&c_k;&c_a;&c_l;&c_y;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_i;&c_g;&c_l;&c_o;&c_t;&c_je;&c_r;&c_a;&c_p;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&#1082;&c_u;&#1087;&c_u;&#1085;&#1082;&c_t;&c_u;&c_r;&c_a;</trd>,
    <trd>&c_g;&#1086;&#1083;&#1082;&#1086;&c_t;&c_je;&c_r;&c_a;&#1087;&c_ib;&#1103;</trd>, 
    <trd>&c_g;&#1086;&#1083;&#1082;&#1086;&c_v;&#1082;&#1086;&#1083;&#1102;&c_v;&c_a;&#1085;&#1085;&#1103;</trd>
  </trdgrp>
</drv>

<drv mrk="akupunktur.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="akupunktur.0isto.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Kuracisto aganta per <tld/>o:
      <ekz>
	li iris &ccirc;ien por ser&ccirc;i kuraciston, fine li trovis <tld/>istinon
	<fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 75a, volumo 2a, p.
	591a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_u;&c_p;&c_u;&c_n;&c_k;&c_t;&c_u;&c_r;&c_n;&c_ib;&c_k;</trd>
  <trd lng="bg">&c_a;&c_k;&c_u;&c_p;&c_u;&c_n;&c_k;&c_t;&c_u;&c_r;&c_i;&c_s;&c_t;</trd>
  <trd lng="de">Akupunkteur</trd>
  <trd lng="en">acupuncturist</trd>
  <trdgrp lng="es">
    <trd>acupuntor</trd>
    <trd>acupunturista</trd>
  </trdgrp>
  <trd lng="fa">&a_m;&a_t;&a_kh;&a_S;&a_S; <ind>&a_T;&a_b; &a_s;&a_w;&a_z;&a_n;&f_y;</ind></trd>
  <trdgrp lng="fr">
    <trd>acuponcteur</trd>,
    <trd>acupuncteur</trd>
  </trdgrp>
  <trd lng="hu">akupunkt&uacute;r&aacute;val gy&oacute;gy&iacute;t&oacute;</trd>
  <trd lng="nl">acupuncturist</trd>
  <trd lng="pl">akupunkturzysta</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_g;&c_l;&c_o;&c_u;&c_k;&c_a;&c_l;&c_y;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_je;&c_c;&c_i;&c_a;&c_l;&c_i;&c_s;&c_t; &c_p;&c_o;
    <ind>&c_i;&c_g;&c_l;&c_o;&c_t;&c_je;&c_r;&c_a;&c_p;&c_i;&c_i;</ind></trd>
  </trdgrp>
</drv>
