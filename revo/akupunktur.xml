<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>akupunktur</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="akupunktur.0o">
  <kap><tld/>o</kap>
  <snc mrk="akupunktur.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Kuraca metodo, kiu uzas pinglojn enpikatajn en la supra ha&ubreve;to
      &ccirc;e difinitaj lokoj de la korpo.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_k;&c_u;&c_p;&c_u;&c_n;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>,
   
<trd>&c_ib;&c_g;&c_o;&c_l;&c_k;&c_a;&c_w;&c_k;&c_o;&c_l;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="es">acupuntura</trd>
  </trdgrp>
  <trd lng="de">Akupunktur</trd>
 <trd lng="en">Acupuncture</trd>
  <trdgrp lng="fr">
    <trd>acuponcture</trd>,
    <trd>acupuncture</trd>
  </trdgrp>
  <trd lng="nl">acupunctuur</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_k;&c_u;&c_p;&c_u;&c_n;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>,
    <trd>&c_i;&c_g;&c_l;&c_o;&c_u;&c_k;&c_a;&c_l;&c_y;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="akupunktur.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="akupunktur.0isto.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Kuracisto aganta per <tld/>o:
      <ekz>
        li iris &ccirc;ien por ser&ccirc;i kuraciston, fine li trovis <tld/>istinon
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 75a, volumo 2a, p.
        591a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Akupunkteur</trd>
  <trd lng="es">acupunturista</trd>
  <trdgrp lng="fr">
    <trd>acuponcteur</trd>,
    <trd>acupuncteur</trd>
  </trdgrp>
  <trd lng="nl">acupuncturist</trd>
</drv>
</art>

