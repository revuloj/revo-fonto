<vortaro>
<art mrk="$Id: epitel.xml,v 1.19 2022/03/17 13:47:19 revo Exp $">
  <kap>
  <rad>epiteli</rad>/o
  </kap>
<drv mrk="epitel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="cxel.0aro">&Ccirc;elaro</ref>, kiu surkovras la supra&jcirc;on
      de mukozoj:
      <ekz>
        la virusoj el la <tld/>o de la intestaro penetras la sangon, de kie ili atingas
        la anta&ubreve;ajn kornojn de la spina medolo kaj la cerebron
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2011/010269.php">Poliomjelito: &ccirc;u la dua tutmonde likvidota infektmalsano?</url></vrk>
        </fnt>.
      </ekz> 
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_p;&c_ib;&c_t;&c_e;&c_l;&c_ib;&c_j;</trd>
  <trd lng="ca">epiteli</trd>
  <trdgrp lng="cs">
    <trd>epitel</trd>,
    <trd>v&yacute;stelka</trd>,
    <trd>zevn&iacute; vrstva sliznice</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Deckzellenschicht</trd>,
     <trd>Epithel</trd>,
     <trd>Epithelium</trd>
  </trdgrp>
  <trd lng="en">epithelium</trd>
  <trd lng="es">epitelio</trd>
  <trd lng="fr">&eacute;pith&eacute;lium</trd>
  <trdgrp lng="hu">
     <trd>fed&odblac;h&aacute;m</trd>,
     <trd>epithelium</trd>
  </trdgrp>
  <trd lng="it">epitelio</trd>
  <trd lng="pl">nab&lstroke;onek</trd>
  <trd lng="pt">epit&eacute;lio</trd>
  <trd lng="ru">&c_e;&c_p;&c_i;&c_t;&c_je;&c_l;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>epitel</trd>,
    <trd>v&yacute;stelka</trd>
  </trdgrp>
  <trd lng="uk">&#x435;&#x43F;&#x456;&#x442;&#x435;&#x43B;&#x456;&#x439;</trd>
</drv>
</art>
