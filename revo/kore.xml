<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id$">
    <kap><rad>kore</rad>/o</kap>
    
    <drv mrk="kore.0o">
      <kap><tld/>o</kap>
      <snc mrk="kore.0o.homo">
        <dif>
          Ano de etno kies historia lo&gcirc;loko estas <tld
          lit="K"/>ujo.
        </dif>
        <trd lng="be">&c_k;&c_a;&c_r;&c_e;&c_je;&c_c;</trd>
        <trd lng="de">Koreaner</trd>
        <trd lng="en">Korean</trd>
        <trd lng="fr">Cor&eacute;en<klr> (habitant)</klr></trd>
        <trd lng="io">Koreano</trd>
        <trd lng="nl">Koreaan</trd>
        <trd lng="pl">Korea&nacute;czyk</trd>
        <trd lng="pt">coreano <klr tip="ind"> (subst.)</klr></trd>
        <trd lng="ru">&c_k;&c_o;&c_r;&c_je;&c_je;&c_c;</trd>
        <trd lng="tp">jan Anku</trd>
        <trd lng="vo">Korey&auml;nan</trd>
      </snc>
      <snc mrk="kore.0o.malsano">
        <uzo tip="fak">MED</uzo>
        <uzo tip="stl">EVI</uzo>
        <ref tip="dif" cel="hxore.0o">&hcirc;oreo</ref>
        <trdgrp lng="nl">
          <trd>choree<klr tip="ind"> (ontr.)</klr></trd>,
          <trd>st.-vitusdans<klr tip="ind"> (ontr.)</klr></trd>
        </trdgrp>
      </snc>
      <trdgrp lng="cs">
        <trd>posun&ccaron;ina</trd>,
        <trd>tanec sv. V&iacute;ta</trd>
      </trdgrp>
      <trd lng="sk">tanec sv. V&iacute;ta</trd>
    </drv>
    
    <drv mrk="kore.0a">
      <kap><tld/>a</kap>
      <snc>
      <dif>
        Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato.
      </dif>
      </snc>
      <trd lng="cs">korejsk&yacute;</trd>
      <trd lng="fr">cor&eacute;en<klr> (adjectif)</klr></trd>
      <trd lng="io">Koreana</trd>
      <trd lng="nl">Koreaans</trd>
      <trd lng="pt">coreano <klr tip="ind"> (adj.)</klr></trd>
      <trd lng="sk">k&oacute;rejsk&yacute;</trd>
      <trd lng="vo">Korey&auml;nik</trd>
    </drv>
    
    <drv mrk="kore.K0ujo">
      <kap><tld lit="K"/>ujo</kap>
      <snc>
      <uzo tip="fak">GEOG</uzo>
      <dif>
        Lando en la <ref tip="malprt" cel="azi.OrientaA0o">Orienta
        Azio</ref>,
        situanta plejparte sur samnoma duoninsulo inter la Japana
        Maro kaj la Maro Flava.
      </dif>
      </snc>
      <trd lng="be">&c_K;&c_a;&c_r;&c_e;&c_ja;</trd>
      <trd lng="ca">Corea</trd>
      <trd lng="cs">Korea</trd>
      <trd lng="de">Korea</trd>
      <trd lng="en">Korea</trd>
      <trd lng="es">Corea</trd>
      <trd lng="fr">Cor&eacute;e</trd>
      <trd lng="fy">Kore&auml;</trd>
      <trd lng="io">Korea</trd>
      <trd lng="nl">Korea</trd>
      <trd lng="pl">Korea</trd>
      <trd lng="pt">Coreia</trd>
      <trd lng="ru">&c_K;&c_o;&c_r;&c_je;&c_ja;</trd>
      <trd lng="sk">K&oacute;rea</trd>
      <trd lng="tp">ma Anku</trd>
      <trd lng="vo">Korey&auml;n</trd>
    </drv>

    <drv mrk="kore.SudK0ujo">
      <kap>
        Sud <tld lit="K"/>ujo
        <fnt>
          <vrk>Oficiala Informo de AdE</vrk>,
          <lok>numero 11</lok>
        </fnt>
      </kap>
      <snc mrk="kore.SudK0ujo.GEOG">
        <uzo tip="fak">GEOG</uzo>
        <uzo tip="fak">POL</uzo>
        <dif>
          &Scirc;tato en la <ref tip="malprt"
          cel="azi.OrientaA0o">Orienta Azio</ref>,
          suda parto de iam unu &scirc;tato - <ref tip="malprt"
          cel="kore.K0ujo">Koreujo</ref>.
        </dif>
        <trd lng="de">S&uuml;dkorea</trd>
        <trd lng="en">South Korea</trd>
        <trd lng="fr">Cor&eacute;e du Sud</trd>
        <trd lng="fy">S&uacute;d-Kore&auml;</trd>
        <trd lng="id"><ind>Korea</ind> Selatan</trd>
        <trd lng="io">Sud-Korea</trd>
        <trd lng="nl">Zuid-<ind>Korea</ind></trd>
        <trd lng="pl">Korea Po&lstroke;udniowa</trd>
        <trd lng="pt">Coreia do Sul</trd>
        <trd lng="tp">ma Anku seli</trd>
        <trd lng="vo">Sul&uuml;da-Korey&auml;n</trd>
      </snc>
    </drv>

    <drv mrk="kore.NordK0ujo">
      <kap>
        Nord <tld lit="K"/>ujo
        <fnt>
          <vrk>Oficiala Informo de AdE</vrk>,
          <lok>numero 11</lok>
        </fnt>
      </kap>
      <snc mrk="kore.NordK0ujo.GEOG">
        <uzo tip="fak">GEOG</uzo>
        <uzo tip="fak">POL</uzo>
        <dif>
          &Scirc;tato en la <ref tip="malprt"
          cel="azi.OrientaA0o">Orienta Azio</ref>,
          norda parto de iam unu &scirc;tato - <ref tip="malprt"
          cel="kore.K0ujo">Koreujo</ref>.
        </dif>
        <trd lng="de">Nordkorea</trd>
        <trd lng="en">North Korea</trd>
        <trd lng="fr">Cor&eacute;e du Nord</trd>
        <trd lng="fy">Noard-Kore&auml;</trd>
        <trd lng="id"><ind>Korea</ind> Utara</trd>
        <trd lng="io">Nord-Korea</trd>
        <trd lng="nl">Noord-<ind>Korea</ind></trd>
        <trd lng="pl">Korea P&oacute;&lstroke;nocna</trd>
        <trd lng="pt">Coreia do Norte</trd>
        <trd lng="tp">ma Anku lete</trd>
        <trd lng="vo">Nol&uuml;da-Korey&auml;n</trd>
      </snc>
    </drv>
  </art>

