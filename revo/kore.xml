<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: kore.xml,v 1.29 2024/10/10 20:31:07 revo Exp $">
    <kap><ofc>10</ofc><rad>kore</rad>/o</kap>

<drv mrk="kore.0o">
  <kap><ofc>10</ofc><tld/>o</kap>
  <snc mrk="kore.0o.homo">
    <dif>
      Ano de etno kies historia lo&gcirc;loko estas <tld lit="K"/>ujo:
      <ekz>
        anta&ubreve; <nom>Gutenberg</nom> <tld/>oj uzis moveblajn literetojn
        por presi
        <fnt>
          <bib>Monato</bib>
          <aut>Mireille Grosjean</aut>
          <vrk><url ref="&Monato;/2010/010072.php">Mi
          koleras!</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        170 <tld/>oj el Suda Koreio ricevis kunvokon al tribunalo por
        ju&gcirc;o la 4an de januaro 1900
        <fnt><aut>Gilbert Ledon</aut>,
        <vrk><url ref="&Monato;/2000/006916.php">Bugra bug-insekto</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/05, p. 5</lok>
        </fnt>;
      </ekz>
      <ekz>
        la a&ubreve;toro, <tld/>o, asertas, ke la japana registaro falsas
        historion,
        neante, ke <nom>Takesima</nom> &ndash; tiel japanoj nomas la insulon
        Tokto &ndash; estas <tld/>a
        teritorio, kaj pretendante, ke &gcirc;i apartenas al Japanio
        <fnt>
          <bib>Monato</bib>
          <aut>Ma Young-tae</aut>
          <vrk><url ref="&Monato;/2005/008651.php">Tokto mankas
          takto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        multaj sud-<tld/>oj deziras reunui&gcirc;on, tion montras la
        paperetoj,
        kiuj trovi&gcirc;as &ccirc;e la muro en la stacidomo de <nom>Imjingang</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>Grosjean Mirejo kaj Lee Jung Kee</aut>
          <vrk><url ref="&Monato;/2008/009657.php">Vundita
          lando</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        homoj tiel brutale subpremataj kaj torturataj per cerbolavado
        kiel nord-<tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2009/009837.php">Monstra
          atompotenco</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_r;&c_e;&c_je;&c_c;</trd>
    <trd lng="de">Koreaner</trd>
    <trd lng="en">Korean</trd>
    <trd lng="fr">Cor&eacute;en<klr> (habitant)</klr></trd>
    <trd lng="io">Koreano</trd>
    <trd lng="nl">Koreaan</trd>
    <trd lng="pl">Korea&nacute;czyk</trd>
    <trd lng="pt">coreano <klr tip="ind"> (subst.)</klr></trd>
    <trd lng="ru">&c_k;&c_o;&c_r;&c_je;&c_je;&c_c;</trd>
    <trdgrp lng="tok">
      <trd>jan pi ma Anku</trd>,
      <trd>jan pi ma Soson</trd>,
      <trd>jan Anku</trd>,
      <trd>jan Soson</trd>
    </trdgrp>
    <trd lng="vo">Korey&auml;nan</trd>
    <trdgrp lng="zh">
    <trd>&#x97D3;&#x570B;&#x4EBA; <pr>h&#xE1;ngu&#xF3;r&#xE9;n</pr></trd>,
    <trd>&#x97D3;&#x570B;&#x4EBA; <pr>H&#xE1;ngu&#xF3;r&#xE9;n</pr></trd>
  </trdgrp>
</snc>
  <ref tip="hom" cel="hxore.0o">&hcirc;oreo, koreo (malsano)</ref>
  <trdgrp lng="cs">
    <trd>posun&ccaron;ina</trd>,
    <trd>tanec sv. V&iacute;ta</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x671D;&#x9BAE;&#x4EBA;  <pr>&#12385;&#12423;&#12358;&#x305B;&#12435;&#12376;&#12435;</pr></trd>,
    <trd>&#x97D3;&#x56FD;&#x4EBA;  <pr>&#x304B;&#12435;&#12371;&#x304F;&#12376;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="sk">tanec sv. V&iacute;ta</trd>
</drv>

<drv mrk="kore.0a">
  <kap><ofc>10</ofc><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tatoj:
      <ekz>
        pli ol 150 lingvoj, inter kiuj la afrikansa kaj la albana, la
        &ccirc;ina, <tld/>a kaj japana, <klr>[&#8230;]</klr>
        <fnt>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="&Monato;/1995/004514.php">Por lingvemaj
          komputilemuloj ...</url></vrk>
          <bib>Monato</bib>
          <lok>1995/06, p. 22</lok>
        </fnt>;
      </ekz>
      <ekz>
        la eltrovinto de la <tld/>a fama tizano <nom>&Gcirc;insen son&gcirc;e</nom>
        <fnt>
          <aut>Reza Kheir-Khah</aut>
            <vrk><url ref="&Monato;/2000/007087.php">Mirinda dimensio de nia vivo</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 16</lok>
          </fnt>;
      </ekz>
      <ekz>
        <tld/>aj turismaj organiza&jcirc;oj kunlaboras kun &ccirc;inaj bankoj
        <fnt>
          <bib>Monato</bib>
          <aut>Xu Jinming</aut>
          <vrk><url ref="&Monato;/2012/010325.php">Alloga&jcirc;oj por &ccirc;inaj
          voja&gcirc;antoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        post la <tld/>a milito sekvis la vjetnama
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj'</aut>
          <vrk><url ref="&Monato;/2012/010869.php">Mond' eterne
          militanta</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>an kulturon po&scirc;tmarke prezentas <tld/>a muziko nomata
        &leftquot;pansori&rightquot;
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2013/011074.php">Slovak-korea
          po&scirc;tmarko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_e;&c_j;&c_s;&c_k;&c_ib;</trd>
  <trd lng="cs">korejsk&yacute;</trd>
  <trd lng="fr">cor&eacute;en<klr> (adjectif)</klr></trd>
  <trd lng="io">Koreana</trd>
  <trd lng="ja">&#x671D;&#x9BAE;&#x30FB;&#x97D3;&#x56FD;&#x306E; 
  <pr>&#12385;&#12423;&#12358;&#x305B;&#12435;&#x30FB;&#x304B;&#12435;&#12371;&#x304F;&#x306E;</pr></trd>
  <trd lng="nl">Koreaans</trd>
  <trd lng="pt">coreano <klr tip="ind"> (adj.)</klr></trd>
  <trd lng="sk">k&oacute;rejsk&yacute;</trd>
  <trd lng="vo">Korey&auml;nik</trd>
</drv>

<drv mrk="kore.K0ujo">
  <kap><ofc>10</ofc><tld lit="K"/>ujo</kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Lando en la <ref tip="malprt" cel="azi.OrientaA0o">Orienta
      Azio</ref>, situanta plej parte sur la <tld lit="K"/>a Duoninsulo,
      sed
      de 1948 dividita en du &scirc;tatojn:
      <ekz>
        fine de sia vivo la imperiestro <nom>Taizon</nom> (mortis en
        649) ekmilitis kontra&ubreve; <tld lit="K"/>ujo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17983">Dinastio
          Tang</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Unu Feri</nom> estis kun mi en <tld lit="K"/>ujo
        <fnt>
          <bib>Monato</bib>
          <aut>Istv&aacute;n Ertl</aut>
          <vrk><url ref="&Monato;/2013/010910.php">Dobos
          Delight</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_K;&c_a;&c_r;&c_e;&c_ja;</trd>
  <trd lng="ca">Corea</trd>
  <trd lng="cs">Korea</trd>
  <trd lng="de">Korea</trd>
  <trd lng="en">Korea</trd>
  <trd lng="es">Corea</trd>
  <trd lng="fr">Cor&eacute;e</trd>
  <trd lng="fy">Kore&auml;</trd>
  <trd lng="io">Korea</trd>
  <trd lng="nl">Korea</trd>
  <trd lng="pl">Korea</trd>
  <trd lng="pt">Coreia</trd>
  <trd lng="ru">&c_K;&c_o;&c_r;&c_je;&c_ja;</trd>
  <trd lng="sk">K&oacute;rea</trd>
  <trd lng="vo">Korey&auml;n</trd>
</drv>

<drv mrk="kore.SudK0ujo">
  <kap>
    <ofc>10</ofc>Sud-<tld lit="K"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="kore.SudK0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&Scirc;tato</ref>
      sur la suda parto de la <tld lit="K"/>a Duoninsulo:
      <ekz>
        <tld lit="K"/>a Respubliko, alinomata anka&ubreve; Sud-Koreio a&ubreve;
        Sud-<tld lit="K"/>ujo
        <klr>[&#8230;]</klr> havas nur unu terlimon, kiu estas 243 km longa
        kaj dividas Sud-Koreion de norda parto Nord-Koreio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3752">Sud-Koreio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_P;&c_a;&c_w;&c_d;&c_n;&c_jo;&c_v;&c_a;&c_ja; <ind>&c_K;&c_a;&c_r;&c_e;&c_ja;</ind></trd>
    <trd lng="de">S&uuml;dkorea</trd>
    <trd lng="en">South Korea</trd>
    <trd lng="fr">Cor&eacute;e du Sud</trd>
    <trd lng="fy">S&uacute;d-Kore&auml;</trd>
    <trd lng="id"><ind>Korea</ind> Selatan</trd>
    <trd lng="io">Sud-Korea</trd>
    <trd lng="nl">Zuid-<ind>Korea</ind></trd>
    <trd lng="pl">Korea Po&lstroke;udniowa</trd>
    <trd lng="pt">Coreia do Sul</trd>
    <trd lng="tok">ma Anku</trd>
    <trd lng="vo">Sul&uuml;da-Korey&auml;n</trd>
    <trdgrp lng="zh">
    <trd>&#21335;&#x97D3; <pr>N&#xE1;nh&#xE1;n</pr></trd>,
    <trd>&#21335;&#x97D3; <pr>n&#xE1;nh&#xE1;n</pr></trd>,
    <trd>&#21335;&#x671D;&#x9BAE; <pr>N&#xE1;nCh&#xE1;oxi&#x1CE;n</pr></trd>
  </trdgrp>
</snc>
</drv>

<drv mrk="kore.NordK0ujo">
  <kap>
    <ofc>10</ofc>Nord-<tld lit="K"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="kore.NordK0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&Scirc;tato</ref>
      sur la norda parto de la <tld lit="K"/>a Duoninsulo:
      <ekz>
        Nord-Koreio a&ubreve; Nord-<tld lit="K"/>ujo, oficiale <tld lit="K"/>a
        Demokratia Popola Respubliko <klr>[&#8230;]</klr> limas la &Ccirc;inan
        Popolan
        Respublikon en la nordo <klr>[&#8230;]</klr> kaj Rusion en la
        nordoriento
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3058">Nord-Koreio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_P;&c_a;&c_w;&c_n;&c_o;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_K;&c_a;&c_r;&c_e;&c_ja;</ind></trd>
    <trd lng="de">Nordkorea</trd>
    <trd lng="en">North Korea</trd>
    <trd lng="fr">Cor&eacute;e du Nord</trd>
    <trd lng="fy">Noard-Kore&auml;</trd>
    <trd lng="id"><ind>Korea</ind> Utara</trd>
    <trd lng="io">Nord-Korea</trd>
    <trd lng="nl">Noord-<ind>Korea</ind></trd>
    <trd lng="pl">Korea P&oacute;&lstroke;nocna</trd>
    <trd lng="pt">Coreia do Norte</trd>
    <trd lng="tok">ma Soson</trd>
    <trd lng="vo">Nol&uuml;da-Korey&auml;n</trd>
    <trdgrp lng="zh">
    <trd>&#21271;&#x671D;&#x9BAE; <pr>b&#x11B;ich&#xE1;oxi&#x1CE;n</pr></trd>,
    <trd>&#21271;&#x671D;&#x9BAE; <pr>B&#x11B;iCh&#xE1;oxi&#x1CE;n</pr></trd>
  </trdgrp>
</snc>
</drv>

