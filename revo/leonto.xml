<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>leontod</rad>/o
</kap>
<drv mrk="leonto.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="astera.0oj">asteracoj</ref>
      (<trd lng="la">Taraxacum</trd>), herbo kun flava floraro kaj
      dentrandaj folioj, uzebla kiel salato.
    </dif>
  </snc>
  <bld lok="../bld/leontod.jpg">
    <ind>ordinara <tld/>o</ind>
    <trd lng="la">Taraxacum officinale</trd>
  </bld>
  <bld lok="../bld/leontod1.jpg">
    fruktaro de ordinara <tld/>o
  </bld>
  <bld lok="../bld/leontod2.jpg">
    floraro de ordinara <tld/>o
  </bld>
  <trd lng="be">&c_d;&c_z;&c_mol;&c_m;&c_u;&c_h;&c_a;&c_v;&c_je;&c_c;</trd>
  <trdgrp lng="de">
    <trd>Kuhblume</trd>,
    <trd>L&ouml;wenzahn</trd>
  </trdgrp>
  <trd lng="en">dandelion</trd>
  <trd lng="fr">pissenlit</trd>
  <trd lng="nl">paardebloem</trd>
  <trd lng="ru">&c_o;&c_d;&c_u;&c_v;&c_a;&c_n;&c_ch;&c_i;&c_k;</trd>
</drv>
</art>
