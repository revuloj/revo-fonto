<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>leontod</rad>/o
</kap>
<drv mrk="leonto.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="aster.0acoj">asteracoj</ref>
      (<trd lng="la">Taraxacum</trd>), herbo kun flava floraro kaj
      dentrandaj folioj, uzebla kiel salato.
    </dif>
  </snc>
  <bld lok="../bld/leontod.jpg">
    ordinara <tld/>o (<trd lng="la">Taraxacum officinale</trd>)
  </bld>
  <bld lok="../bld/leontod1.jpg">
    fruktaro, (<trd lng="la">Taraxacum officinale</trd>)
  </bld>
  <bld lok="../bld/leontod2.jpg">
    floraro, (<trd lng="la">Taraxacum officinale</trd>)
  </bld>
  <trdgrp lng="de">
    <trd>Kuhblume</trd>,
    <trd>L&ouml;wenzahn</trd>
  </trdgrp>
  <trd lng="en">dandelion</trd>
  <trd lng="fr">pissenlit</trd>
  <trd lng="nl">paardebloem</trd>
</drv>
</art>
