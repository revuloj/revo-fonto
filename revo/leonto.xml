<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: leonto.xml,v 1.21 2024/01/15 16:14:09 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>leontod</rad>/o
</kap>
<drv mrk="leonto.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio <ref tip="super" cel="astera.0oj">asteracoj</ref>
      (<trd lng="la">Taraxacum</trd>), herbo kun flava floraro kaj
      dentrandaj folioj, uzebla kiel salato:
      <ekz>
        <nom>Gerda</nom> iris al la <tld/>o, kiu lumetis tra siaj verdaj folioj
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        violoj staris en plena florado, la anemonoj kaj <tld/>oj disvolvi&gcirc;is
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de la jaro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/leontod.jpg">
    <ind>ordinara <tld/>o</ind>
    <trd lng="la">Taraxacum officinale</trd>
    <trdgrp lng="pl">
      <trd>mniszek lekarski</trd>,
      <trd>mniszek pospolity</trd>
    </trdgrp>
  </bld>
  <bld lok="../bld/leontod1.jpg">
    fruktaro de ordinara <tld/>o
  </bld>
  <bld lok="../bld/leontod2.jpg">
    floraro de ordinara <tld/>o
  </bld>
  <trd lng="be">&c_d;&c_z;&c_mol;&c_m;&c_u;&c_h;&c_a;&c_v;&c_je;&c_c;</trd>
  <trd lng="br">c'hwervizon<klr> [pl.]</klr></trd>
  <trdgrp lng="de">
    <trd>Kuhblume</trd>,
    <trd>L&ouml;wenzahn</trd>
  </trdgrp>
  <trd lng="en">dandelion</trd>
  <trd lng="fr">pissenlit</trd>
  <trdgrp lng="hu">
    <trd>pitypang</trd>,
    <trd>gyermekl&aacute;ncf&udblac;</trd>,
    <trd>kutyatej</trd>
  </trdgrp>
  <trd lng="ja">&#x30BF;&#x30F3;&#x30DD;&#x30DD;</trd>
  <trd lng="nl">paardebloem</trd>
  <trd lng="pl">mniszek</trd>
  <trd lng="pt">dente-de-le&atilde;o</trd>
  <trd lng="ru">&c_o;&c_d;&c_u;&c_v;&c_a;&c_n;&c_ch;&c_i;&c_k;</trd>
  <trd lng="uk">&c_k;&c_u;&c_l;&c_mol;&c_b;&c_a;&c_b;&c_a;</trd>
  <trd lng="zh">&#x84B2;&#x516C;&#x82F1; <pr>p&#xFA;g&#x14D;ngy&#x12B;ng</pr></trd>
</drv>
</art>
