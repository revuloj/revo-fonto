<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kariol.xml,v 1.16 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>kariol</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kariol.0o">
  <kap><tld/>o</kap>
  <snc mrk="kariol.0o.TRA">
    <uzo tip="fak">TRA</uzo>  
    <dif>
      Durada malpeza nekovrita &ccirc;evaltirata
      <ref tip="super" cel="vetur.0ilo.TRA">veturilo</ref>:
      <ekz>   
        ili sidis en <tld/>o resumante la rezulton
        de la taga laboro
        <fnt>
          <aut>A. &Ccirc;e&hcirc;ov, trad. A. Kor&jcirc;enkov</aut>,
          <vrk><url
          ref="http://www.esperanto-ondo.ru/Libroj/V_bibl02.pdf">
          Sveda alumeto</url></vrk>,
          <lok>2004-2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_v;&c_u;&c_k;&c_o;&c_l;&c_k;&c_a;</trd>
  <trd lng="ca">cabriol&eacute;</trd>
  <trdgrp lng="cs">
    <trd>dvojkolka</trd>,
    <trd>kariola</trd>
  </trdgrp>
  <trd lng="de">Kariole</trd>
  <trdgrp lng="en">
    <trd>dog cart</trd>,
    <trd>trap</trd>
  </trdgrp>
  <trd lng="fr">carriole</trd>
  <trd lng="hu">kord&eacute;</trd>
  <trd lng="nl">sjees</trd>
  <trd lng="pl">kariolka</trd>
  <trd lng="ru">&c_d;&c_v;&c_u;&c_k;&c_o;&c_l;&c_k;&c_a;</trd>
  <trd lng="sk">dvojkolesový ľahký voz</trd>
</drv>
</art>
