<art mrk="$Id: kariol.xml,v 1.22 2023/12/03 08:00:23 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>kariol</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="kariol.0o">
  <kap><tld/>o</kap>
  <snc mrk="kariol.0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      Durada malpeza nekovrita &ccirc;evaltirata
      <ref tip="super" cel="vetur.0ilo.TRA">veturilo</ref>:
      <ekz>
        survoje hejmen en <tld/>o, kiun ili estis postlasintaj &ccirc;e la vila&gcirc;a hotelo
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili sidis en <tld/>o resumante la rezulton
        de la taga laboro
        <fnt>
          <aut>A. &Ccirc;e&hcirc;ov, trad. A. Kor&jcirc;enkov</aut>,
          <vrk><url ref="&LOdE;Libroj/V_bibl02.pdf">
          Sveda alumeto</url></vrk>,
          <lok>2004-2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/0/00/Eschke_1kl.jpg/550px-Eschke_1kl.jpg" prm="CC BY-SA 2.0 de"><tld/>a po&scirc;to
        <fnt>
          <aut>Richard Eschke, Muzea Fonda&jcirc;o por Po&scirc;to kaj Telekomunikado</aut>
          <lok><url ref="&WCW;/File:Eschke_1kl.jpg">https://commons.wikimedia.org/wiki/File:Eschke_1kl.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_d;&c_v;&c_u;&c_k;&c_o;&c_l;&c_k;&c_a;</trd>
  <trd lng="ca">cabriol&eacute;</trd>
  <trdgrp lng="cs">
    <trd>dvojkolka</trd>,
    <trd>kariola</trd>
  </trdgrp>
  <trd lng="de">Karriol</trd>
  <trdgrp lng="en">
    <trd>dog cart</trd>,
    <trd>trap</trd>
  </trdgrp>
  <trd lng="fr">carriole</trd>
  <trd lng="hu">kord&eacute;</trd>
  <trd lng="ja">&#x4E8C;&#x8F2A;&#x99AC;&#x8ECA;  <pr>&#x306B;&#x308A;&#x3093;&#x3070;&#x3057;&#x3083;</pr></trd>
  <trd lng="nl">sjees</trd>
  <trd lng="pl">kariolka</trd>
  <trd lng="ru">&c_d;&c_v;&c_u;&c_k;&c_o;&c_l;&c_k;&c_a;</trd>
  <trd lng="sk">dvojkolesov&yacute; &lcaron;ahk&yacute; voz</trd>
  <trdgrp lng="uk">
    <trd>&c_l;&c_je;&c_g;&c_k;&c_i;&c_j; &c_d;&c_v;&c_o;&c_k;&c_o;&c_l;&c_ib;&c_s;&c_n;&c_i;&c_j; &c_je;&c_k;&c_ib;&c_p;&c_a;&c_zh;</trd>,
    <trd>&c_b;&c_r;&c_i;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#x8F2B; <pr>p&#xE1;i</pr></trd>
</drv>
</art>
