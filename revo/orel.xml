<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc> <rad>orel</rad>/o</kap>
<drv mrk="orel.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="orel.0o.ANA">
     <uzo tip="fak">ANA</uzo>
     <dif>
       Organo de a&ubreve;dkapablo:
       <ekz>
         kapti hundon je la <tld/>oj<fnt>Z</fnt>;
       </ekz>
       <ekz>
         la &ccirc;evalo <ind>akrigas la <tld/>ojn</ind><fnt>Z</fnt>
         <klr>(ekatentas)</klr>;
         <trd lng="hu">hegyezi a f&uuml;l&eacute;t</trd>         
       </ekz>
       <ekz>
         stre&ccirc;i la <tld/>on<fnt>Z</fnt>;
       </ekz>
       <ekz>
         klini la <tld/>on al ies parolo<fnt>Z</fnt>;
       </ekz>
       <ekz>
         forklini sian <tld/>on de a&ubreve;dado de prudento<fnt>Z</fnt>;
       </ekz>
       <ekz>
         <ind>tra unu <tld/>o eniras, tra la dua eliras</ind>
         <fnt><bib>PrV</bib></fnt>;
         <trd lng="hu">az egyik f&uuml;l&ouml;n be, a m&aacute;sokon ki</trd>         
       </ekz>
       <ekz>
         paroli, diri en ies <tld/>on<fnt>Z</fnt>
         <klr>(sekreti)</klr>;
       </ekz>
       <ekz>
         ne estas bezone, ke oni tiru lin per <tld/>o<fnt>Z</fnt>
         <klr>(ke oni admonu lin)</klr>.
       </ekz>
     </dif>
     <trd lng="de">Ohr</trd>
     <trdgrp lng="pt">
       <trd>orelha</trd>;
       <trd>ouvido</trd>
     </trdgrp>
   </snc>
   <snc mrk="orel.0o.FIG">
     <uzo tip="stl">FIG</uzo>
     <dif>
       A&ubreve;dado, a&ubreve;dkapablo:
       <ekz>
         malsata stomako <tld/>on ne havas
         <klr>(ne a&ubreve;das)</klr>
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         la muro havas <tld/>ojn
         <klr>(estas a&ubreve;skultanto post &gcirc;i)</klr>
         <fnt><bib>PrV</bib></fnt>;
         <trd lng="he">&alef;&vav;&zayin;&nun;&yod;&yod;&fmem; &lamed;&kaf;&vav;&tav;&lamed;</trd>
       </ekz>
       <ekz>
         tiu muziko tran&ccirc;as la <tld/>on<fnt>Z</fnt>;
       </ekz>
       <ekz>
         ju&gcirc;anto devas havi du <tld/>ojn
         <klr>(a&ubreve;skulti egale amba&ubreve; ju&gcirc;otojn)</klr>
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         se tio venus al liaj <tld/>oj<fnt>Z</fnt>.
       </ekz>
     </dif>
     <trd lng="de">Geh&ouml;hr</trd>
     <trd lng="pt">ouvido</trd>
   </snc>                     
   <trd lng="be">&c_v;&c_u;&c_h;&c_a;</trd>
   <trd lng="br">skouarn</trd>
   <trd lng="cs">ucho</trd>
   <trd lng="de">Ohr</trd>
   <trd lng="en">ear</trd>
   <trd lng="fr">oreille</trd>
   <trd lng="he">&alef;&vav;&zayin;&fnun;</trd>
   <trd lng="hr">uho</trd>
   <trd lng="hu">f&uuml;l</trd>   
   <trd lng="nl">oor</trd>
   <trd lng="pl">ucho</trd>
   <trd lng="ru">&c_u;&c_h;&c_o;</trd>
   <trd lng="sv">&ouml;ra</trd>
</drv>

<drv mrk="orel.0a">
   <kap><tld/>a</kap>
   <snc>
     <dif>
       Rilata al <tld/>o:
       <ekz>
         <tld/>a konko;
       </ekz>
       <ekz>
         bu&scirc;a (perbu&scirc;a) kaj <tld/>a (per<tld/>a) literaturo<fnt>Z</fnt>.
       </ekz>
     </dif>
   </snc>                      
   <trd lng="be">&c_v;&c_u;&c_sh;&c_n;&c_y;</trd>
   <trd lng="br">-<ind>skouarn</ind></trd>
   <trd lng="hu">f&uuml;l-</trd> 
   <trd lng="fr">auriculaire<klr tip="amb"> (adj.)</klr></trd>  
   <trd lng="nl">oor-</trd>
   <trd lng="pl">uszny</trd>
   <trd lng="pt">&oacute;tico</trd>
   <trd lng="ru">&c_u;&c_sh;&c_n;&c_o;&c_j;</trd>
</drv>

<drv mrk="orel.0ringo">
   <kap><tld/>ringo</kap>
   <snc>
     <dif>
       Juvelo portata &ccirc;e la <tld/>oj kiel ornamo.
     </dif>
   </snc>
   <trd lng="be">&c_z;&c_a;&c_v;&c_u;&c_sh;&c_n;&c_ib;&c_c;&c_a;</trd>     
   <trd lng="br">kloc'hig-skouarn</trd>
   <trd lng="de">Ohrring</trd>
   <trd lng="fr">boucle d'<ind>oreille</ind></trd>
   <trd lng="he">&ayin;&gimel;&yod;&lamed;</trd>
   <trd lng="hu">f&uuml;lkarika</trd>   
   <trd lng="nl">oorring</trd>
   <trd lng="pl">kolczyk</trd>
   <trd lng="pt">brinco</trd>
   <trdgrp lng="ru">
     <trd>&c_s;&c_je;&c_r;&c_mol;&c_g;&c_a;</trd>,
     <trd>&c_s;&c_je;&c_r;&c_jo;&c_zh;&c_k;&c_a;</trd>
   </trdgrp>
   <trd lng="sv">&ouml;rh&auml;nge</trd>
</drv>

<drv mrk="orel.0tamburo">
   <kap><tld/>tamburo<fnt>Z</fnt></kap>
   <uzo tip="fak">ANA</uzo>
   <snc>
     <dif>
       <ref tip="dif" cel="timpan.0o.ANA">Timpano</ref>.
     </dif>
   </snc>
   <trd lng="be">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n;&c_n;&c_a;&c_ja; &c_p;&c_je;&c_r;&c_a;&c_p;&c_o;&c_n;&c_k;&c_a;</trd>    
   <trd lng="br">taboulin<klr tip="ind"> (ar skouarn)</klr></trd>
   <trd lng="de">Trommelfell</trd>   
   <trd lng="fr">tympan</trd>
   <trdgrp lng="he">
     <trd>&tav;&vav;&fpe; &he;&alef;&vav;&zayin;&fnun;</trd>,
     <trd>&ayin;&vav;&resh; &he;&tav;&vav;&fpe;</trd>
   </trdgrp>
   <trd lng="hu">dobh&aacute;rtya</trd>   
   <trd lng="nl">trommelvlies</trd>
   <trd lng="pl">b&eogonek;benek uszny</trd>
   <trd lng="pt">t&iacute;mpano</trd>
   <trd lng="ru">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n;&c_n;&c_a;&c_ja; &c_p;&c_je;&c_r;&c_je;&c_p;&c_o;&c_n;&c_k;&c_a;</trd>
</drv>
</art>
