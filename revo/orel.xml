<drv mrk="orel.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        trompite per tiu <tld/>a iluzio, obstinante en direkton kontra&ubreve;an, ili fine tute
        perdi&gcirc;is
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        havantaj literaturon skriban <klr>[&#x2026;]</klr>, bu&scirc;an kaj <tld/>an
        <fnt>
          <bib>LR</bib>
          <lok>Dua Parto &ndash; Gramatiko</lok>
        </fnt>;
      </ekz>
      <ekz>
        malsek<tld/>aj buboj kaptas frazojn el la batalo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        griza pint&#x2019;<tld/>a virhundo
        <fnt>
          <bib>MkM</bib>
          <lok>26</lok>
        </fnt>;
      </ekz>
      <ekz>
        long<tld/>a hundeto
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_sh;&c_n;&c_y;</trd>
  <trd lng="bg">&c_u;&c_sh;&c_je;&c_n;</trd>
  <trd lng="br">-<ind>skouarn</ind></trd>
  <trd lng="cs">u&scaron;n&iacute;</trd>
  <trdgrp lng="en">
    <trd>aural</trd>,
    <trd>auricular</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>auricular</trd>,
    <trd>auditivo</trd>
  </trdgrp>
  <trd lng="fr">auriculaire<klr tip="amb"> (adj.)</klr></trd>
  <trd lng="hu">f&uuml;l-</trd>
  <trd lng="id">aural</trd>
  <trdgrp lng="it">
    <trd> dell'<ind>orecchio</ind></trd>,
    <trd>auricolare<klr> (agg.)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x8033;&#x306E;  <pr>&#x307F;&#x307F;&#x306E;</pr></trd>
  <trd lng="nl">oor-</trd>
  <trd lng="pl">uszny</trd>
  <trd lng="pt">&oacute;tico</trd>
  <trd lng="ru">&c_u;&c_sh;&c_n;&c_o;&c_j;</trd>
  <trd lng="sk">u&scaron;n&yacute;</trd>
  <trd lng="uk">&c_v;&c_u;&c_sh;&c_n;&c_i;&c_j;</trd>
</drv>

<drv mrk="orel.0umo">
  <kap>
    <tld/>umo
  </kap>
  <snc>
    <dif>
      Parto de vesto (a&ubreve; alia objekto) kovranta la <tld/>on:
      <ekz>
        &ccirc;apo kun <tld/>umoj
        <fnt>
          <bib>SrT</bib>
          <lok>Libro VI.</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="auxd.0ilo">a&ubreve;dilo</ref>,
      <ref cel="auxsku.0ilo">a&ubreve;skultilo</ref>
    </refgrp>
  </snc>
  <trd lng="de">Ohrenklappe</trd>
  <trdgrp lng="ja">
    <trd>&#x8033;&#x304A;&#x304A;&#x3044;  <pr>&#x307F;&#x307F;&#x304A;&#x304A;&#x3044;</pr></trd>,
    <trd>&#x53D7;&#x8A71;&#x5668;  <pr>&#x3058;&#x3085;&#x308F;&#x304D;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="orel.0bruo">
  <kap><tld/>bruo</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      &Gcirc;ena elorela bruo kiun iuj personoj povas a&ubreve;de
      percepti kaj kiu
      ne devenas de ekstera &ccirc;irka&ubreve;anta sono.
    </dif>
    <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_v;&c_ib;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je; &c_w; &c_v;&c_u;&c_sh;&c_a;&c_h;</trd>
  <trd lng="bg">&c_sh;&c_u;&c_m; &c_v; &c_u;&c_sh;&c_i;&c_t;&c_je;</trd>
  <trd lng="cs">u&scaron;n&iacute; &scaron;um</trd>
  <trdgrp lng="de">
    <trd>Tinnitus</trd>,
    <trd>Ohrger&auml;usch</trd>
  </trdgrp>
  <trd lng="en">tinnitus</trd>
  <trdgrp lng="es">
    <trd>tinnitus</trd>,
    <trd>ac&uacute;fenos</trd>
  </trdgrp>
  <trd lng="fr">acouph&egrave;ne</trd>
  <trd lng="id">tinitus</trd>
  <trd lng="it">acufene</trd>
  <trd lng="sk">&scaron;um v u&scaron;iach</trd>
  <trdgrp lng="zh">
    <trd>&#x8033;&#x9CF4; <pr>&#x11B;rm&#xED;ng</pr></trd>,
    <trd>&#x8033;&#x9E23; <pr>&#x11B;rm&#xED;ng</pr></trd>,
    <trd>&#x804E; <pr>ti&#x101;o</pr></trd>
  </trdgrp>
</drv>
