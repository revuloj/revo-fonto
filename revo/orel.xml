<drv mrk="orel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="orel.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Organo de a&ubreve;dkapablo:
      <ekz>
        bonodora fojno igis &ccirc;iun &ccirc;evalon, kiu preteriris,
        <ind>akrigi la <tld/>ojn</ind>
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
        <trd lng="de">die <ind>Ohr</ind>en spitzen</trd>
        <trd lng="hu">hegyezi a f&uuml;l&eacute;t</trd>
      </ekz>
      <ekz>
        ili ne a&ubreve;skultis kaj ne alklinis sian <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 7:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> forklinas sian orelon de a&ubreve;dado de instruo
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 28:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu sin miksas en malpropran disputon, estas kiel iu,
        kiu kaptas hundon je la <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 26:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        kion vi parolis en <tld/>on en internaj &ccirc;ambroj, tio estos proklamita sur la
        tegmentoj
        <fnt>
          <bib>NT</bib>
          <lok>Luko 12:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ordonadis al ni meti la <tld/>on al la tero
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Eternulo sendis min al vi, por diri en viajn <tld/>ojn &ccirc;iujn tiujn vortojn
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 26:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu havas <tld/>on, tiu a&ubreve;skultu
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 2:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi a&ubreve;dis tion per miaj propraj <tld/>oj
        <fnt>
          <bib>Fab3</bib>
          <lok>Estas tute certe</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Margarita</nom> sidis &scirc;topinte la <tld/>ojn per la fingroj kaj rigardis
        <fnt>
          <bib>MkM</bib>
          <lok>24</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>tra unu <tld/>o eniras, tra la dua eliras</ind>
        <trd lng="fr">entrer par une oreille et sortir par l'autre</trd>
        <trd lng="hu">az egyik f&uuml;l&ouml;n be, a m&aacute;sokon
        ki</trd>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>stre&ccirc;i la <tld/>on</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>.
        <trd lng="fr">tendre l'oreille</trd>
      </ekz>
    </dif>
    <trd lng="de">Ohr</trd>
    <trdgrp lng="id">
      <trd>kuping</trd>,
      <trd>telinga</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>orelha</trd>;
      <trd>ouvido</trd>
    </trdgrp>
  </snc>
  <snc mrk="orel.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref cel="auxd.0ado">A&ubreve;dado</ref>, a&ubreve;dkapablo:
      <ekz>
        li diable furiozus, se io simila venus al liaj <tld/>oj
        <fnt>
          <bib>GDn</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vortoj slavaj, kiuj, enmetite en sistemon romanagermanan, tran&ccirc;as la orelon
        <fnt>
          <bib>LR</bib>
          <lok>Unua Parto &ndash; Leksikologio</lok>
        </fnt>;
      </ekz>
      <ekz>
        venos al vi forsavi&gcirc;into, por transdoni sciigon al viaj <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 24:26</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>malsata stomako <tld/>on ne havas</ind>
        <trd lng="fr">ventre affam&eacute; n'a pas d'oreille</trd>
        <klr>(ne a&ubreve;das)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>la muro havas <tld/>ojn</ind>
        <klr>(estas a&ubreve;skultanto post &gcirc;i)</klr>
        <trd lng="he">&alef;&vav;&zayin;&nun;&yod;&yod;&fmem;
        &lamed;&kaf;&vav;&tav;&lamed;</trd>
        <trd lng="es">las paredes oyen</trd>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ju&gcirc;anto devas havi du <tld/>ojn
        <klr>(a&ubreve;skulti egale amba&ubreve; ju&gcirc;otojn)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Geh&ouml;hr</trd>
    <trdgrp lng="id">
      <trd><baz>dengar</baz>pendengaran</trd>,
      <trd>rungu</trd>
    </trdgrp>
    <trd lng="pt">ouvido</trd>
  </snc>
