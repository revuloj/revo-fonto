<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ka&jcirc;ol</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="kajxol.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="kares.0i">Karesi</ref>, ame prizorgi, peni por
      pla&ccirc;i al iu, ofte por ricevi ian favoron:
      <ekz>
        <tld/>i infanon;
      </ekz>
      <ekz>
        ili <klr>...</klr> tiom <tld/>as al la ekonomoj
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 71a, volumo 2a, p.
        502a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxarm.0i">&ccirc;armi</ref>,
      <ref cel="flat.0i">flati</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>cajoler</trd>,
    <trd>dorloter</trd>,
    <trd>enj&ocirc;ler</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>babusgat</trd>,
    <trd>cir&oacute;gat</trd>,
    <trd>d&eacute;delget</trd>,
    <trd>gazsul&aacute;l</trd>,
    <trd>h&iacute;zelkedik</trd>
  </trdgrp>
</drv>

<drv mrk="kajxol.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Kun <tld/>a konduto, gestoj, vo&ccirc;o:
      <ekz>
        sed kial li min bezonas? &ndash; <tld/>e demandis
        Margarita
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 19a, p. 197a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">c&acirc;lin<klr> (adv.)</klr></trd>
  <trdgrp lng="hu">
    <trd>babusgatva</trd>,
    <trd>d&eacute;delgetve</trd>,
    <trd>h&iacute;zelg&odblac;n</trd>
  </trdgrp>
</drv>
</art>
