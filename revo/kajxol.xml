<vortaro>
<art mrk="$Id: kajxol.xml,v 1.24 2024/01/29 15:23:00 revo Exp $">
<kap>
   <rad>ka&jcirc;ol</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="kajxol.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc>
     <dif>
       Provi <ref tip="super" cel="inklin.0igi">inklinigi</ref>
       iun per flatado, karesado, afablaj vortoj ks:
      <ekz>
        estis <nom>Gladstone</nom>, kiu <tld/>is la re&gcirc;inon reaperi al la publiko en 1872 por venki
        respublikismon
        <fnt>
          <bib>Monato</bib>
          <aut>W. H. Simcock</aut>
          <vrk><url ref="&Monato;/2005/008702.php">Monarkia mistiko</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi tro <tld/>as &scirc;in, &scirc;i sen&ccirc;ese plorgrimacas kaj vi permesas <klr>[al &scirc;i]</klr> konduki vin
        per
        la nazo
        <fnt>
          <bib>LOdE</bib>
          <aut>Ewa Grochowska</aut>
          <vrk><url ref="&LOdE;Ondo/Lo-308demo.pdf">Ferioj &ccirc;e la maro</url></vrk>
          <lok> 8 A&ubreve;gusto 2021 </lok>
        </fnt>;
      </ekz>
      <ekz>
         ili <klr>[...]</klr> tiom <tld/>as al la ekonomoj
         <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 71a, volumo 2a, p.
         502a</lok></fnt>;
      </ekz>
      <ekz>
        la petskriboj kaj signoj de korinklino de tri e&ubreve;ropaj gvidantoj <klr>[&#8230;]</klr>,
        kiuj <tld/>is s-ron <nom>Donald Trump</nom>, servis al nenio
        <fnt>
          <bib>MD17</bib>
          <vrk>E&ubreve;ropaj piedmatoj</vrk>
        </fnt>.
      </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="cxarm.0i">&ccirc;armi</ref>,
       <ref cel="dorlot.0i.iun">dorloti</ref>,
       <ref cel="flat.0i">flati</ref>,
       <ref cel="konsol.0i.iu">konsoli</ref>
     </refgrp>
   </snc>
   <trdgrp lng="be">
     <trd>&c_p;&c_je;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;</trd>,
     <trd>&c_l;&c_a;&c_sh;&c_ch;&c_y;&c_c;&c_mol;</trd>,
     <trd>&c_d;&c_a;&c_g;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>
   </trdgrp>
   <trd lng="bg">&c_l;&c_a;&c_s;&c_k;&c_a;&c_ja;</trd>
   <trdgrp lng="de">
    <trd>umschmeicheln</trd>,
    <trd>freundlich <ind>stimmen</ind></trd>,
    <trd><ind>gewogen</ind> stimmen</trd>,
    <trd>gut <ind>zureden</ind></trd>,
    <trd>umgarnen</trd>,
    <trd>um den <ind>Bart</ind> gehen</trd>
  </trdgrp>
   <trd lng="en">cajole</trd>
   <trdgrp lng="fi">
     <trd>mairitella (suostumaan)</trd>,
     <trd>suostutella (hell&auml;sti)</trd>,
     <trd>mielistell&auml;</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>cajoler</trd>,
     <trd>c&acirc;liner</trd>,
     <trd>dorloter</trd>,
     <trd>enj&ocirc;ler</trd>
   </trdgrp>
   <trdgrp lng="es">
    <trd>mimar (esp)</trd>,
    <trd>apapachar (amer)</trd>,
    <trd>engatusar</trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>babusgat</trd>,
     <trd>cir&oacute;gat</trd>,
     <trd>d&eacute;delget</trd>,
     <trd>gazsul&aacute;l</trd>,
     <trd>h&iacute;zelkedik</trd>
   </trdgrp>
   <trdgrp lng="ja">
    <trd>&#x6A5F;&#x5ACC;&#12434;&#12392;&#x308B;  <pr>&#x304D;&#12370;&#12435;&#12434;&#12392;&#x308B;</pr></trd>,
    <trd>&#29976;&#12360;&#x308B;  <pr>&#12354;&#x307E;&#12360;&#x308B;</pr></trd>,
    <trd>&#x305F;&#12425;&#12375;&#x8FBC;&#12416;  <pr>&#x305F;&#12425;&#12375;&#12371;&#12416;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
     <trd>vleien</trd>,
     <trd>zoete <ind>broodjes</ind> bakken</trd>
   </trdgrp>
   <trdgrp lng="pl">
     <trd>przymila&cacute; si&eogonek;</trd>,
     <trd>podlizywa&cacute; si&eogonek;</trd>
   </trdgrp>
   <trd lng="pt">bajular</trd>
   <trdgrp lng="ro">
     <trd>lingu&#537;i</trd>,
     <trd>adula</trd>,
     <trd>m&abreve;guli</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_l;&c_mol;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>,
     <trd>&c_p;&c_o;&c_d;&c_l;&c_i;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
     <trd>&c_u;&c_g;&c_o;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_u;&c_g;&c_o;&c_d;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>
   </trdgrp>
   <trdgrp lng="tr">
     <trd>ok&scedil;amak</trd>,
     <trd>sevmek</trd>,
     <trd>tavlamak</trd>,
     <trd>piyazlamak</trd>
   </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_ib;&c_d;&#x43B;&c_je;&c_shch;&c_u;&c_v;&c_a;&c_t;&c_i; (&#x43A;&#x43E;&c_g;&#x43E;-&#x43D;.)</trd>,
    <trd>&#x43F;&c_ib;&c_d;&#x43B;&c_je;&c_shch;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F; (&c_d;&#x43E; &#x43A;&#x43E;&c_g;&#x43E;-&#x43D;.)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x629A;&#25720; <pr>f&#x1D4;m&#x14D;</pr></trd>,
    <trd>&#x8B06; <pr>sh&#xE0;n</pr></trd>,
    <trd>&#x8FDE;&#x54C4;&#x5E26;&#x9A97; <pr>li&#xE1;nh&#x1D2;ngd&#xE0;ipi&#xE0;n</pr></trd>,
    <trd>&#x54EB; <pr>z&#xFA;</pr></trd>,
    <trd>&#x8AC2; <pr>ch&#x1CE;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kajxol.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="K"/>anta:
      <ekz>
        <tld/>a gesto, parolo:
      </ekz>
      <ekz>
        <nom>Nastenjka</nom>! &mdash; mi ekparolis per timida kaj <tld/>a vo&ccirc;o, &mdash; <klr>[&#8230;]</klr> je Dio,
        ne ploru!
        <fnt>
          <aut>Fjodor Dostojevskij</aut>
          <vrk><url ref="&LOdE;Libroj/V_bibl04.pdf">Blankaj noktoj</url></vrk>
          <lok>Historio de Nastenjka</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi volus peti vin ke tio restu inter ni, &ndash; <tld/>miene diris Ste&ccirc;jo
        <fnt>
          <bib>MkM</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>einschmeichelnd</trd>,
    <trd>umgarnend</trd>
  </trdgrp>
  <trdgrp lng="fi">
     <trd>hellytt&auml;v&auml;</trd>,
     <trd>mairea</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>cajoleur<klr tip="ind"> (adj.)</klr></trd>,
     <trd>enjoleur<klr tip="ind"> (adj.)</klr></trd>,
     <trd>c&acirc;lin<klr tip="ind"> (adj.)</klr></trd>
   </trdgrp>
   <trdgrp lng="es">
     <trd>zalamero<klr tip="ind"> (adj.)</klr></trd>,
     <trd>adulador<klr tip="ind"> (adj.)</klr></trd>,
     <trd>mimoso</trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>babusgatva</trd>,
     <trd>d&eacute;delgetve</trd>,
     <trd>h&iacute;zelg&odblac;n</trd>
   </trdgrp>
   <trdgrp lng="ja">
    <trd>&#12408;&#12388;&#12425;&#12356;&#x306E;</trd>,
    <trd>&#x304A;&#12418;&#x306D;&#x308B;&#12424;&#12358;&#x306A;</trd>,
    <trd>&#29976;&#12356;  <pr>&#12358;&#x307E;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">vleiend</trd>
   <trd lng="pl">lizusowy</trd>
    <trdgrp lng="ru">
     <trd>&c_l;&c_mol;&c_s;&c_t;&c_i;&c_v;&c_y;&c_j;</trd>,
     <trd>&c_u;&c_g;&c_o;&c_d;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>,
     <trd>&c_p;&c_o;&c_d;&c_h;&c_a;&c_l;&c_i;&c_m;&c_s;&c_k;&c_i;&c_j;</trd>
   </trdgrp>
   <trdgrp lng="tr">
     <trd>pohpoh&ccedil;u</trd>,
     <trd>seven</trd>
   </trdgrp>
</drv>
</art>
