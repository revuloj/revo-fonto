<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ne&ubreve;tron</rad>/o
</kap>
<drv mrk="neuxtr2.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="nukleon.0o">Nukleono</ref>
      kun maso 1840-oble pli granda ol tiu de elektrono,
      <ref tip="vid" cel="spin2.0o">spino</ref> 1/2
      kaj nula elektra &scirc;argo:
      <ekz>
        libera <tld/>o ne estas stabila, sed post kelkaj minutoj
        spontanee diseri&gcirc;as al <ref cel="proton.0o">protono</ref>,
        <ref cel="elektr14.0o">elektrono</ref> kaj
        <ref cel="neuxtr3.0o">ne&ubreve;trino</ref>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="atom.0o">atomo</ref>
    <ref tip="prt" cel="kvark.0o">kvarko</ref>
    <refgrp tip="vid">
      <ref cel="barjon.0o">barjono</ref>,
      <ref cel="partik2.0o">partiklo</ref>
    </refgrp>
  </snc>
  <trd lng="de">Neutron</trd>
  <trd lng="el">&omikron;&ypsilon;&delta;&epsilon;&tau;&epsilon;&rho;&omikron_ton;&ny;&jota;&omikron;</trd>
  <trd lng="en">neutron</trd>
  <trd lng="es">neutr&oacute;n</trd>
  <trd lng="fr">neutron</trd>
  <trd lng="hu">neutron</trd>
  <trd lng="nl">neutron</trd>
  <trd lng="pt">n&ecirc;utron</trd>
  <trd lng="ru">&c_n;&c_je;&c_j;&c_t;&c_r;&c_o;&c_n;</trd>
</drv>
</art>
