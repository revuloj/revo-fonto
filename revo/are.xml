<drv mrk="are.0i">
  <kap>
    <tld/>i
  </kap>
  <snc mrk="are.0i.KOMUNE">
    <dif>
      <ref tip="vid" cel="amplek.0i">Ampleksi</ref>,
      plene <ref tip="vid" cel="okup.0i.lokigxi">okupi<sncref/></ref>:
      <ekz>
        la servejo <tld/>as proksimume 40 kvadratajn metrojn, estas orda kaj pura
        <fnt>
          <aut>&Gcirc;oja</aut>
          <vrk><url ref="&CRI;2521/2018/07/31/165s200237.htm">Unu tago de &gcirc;enerala kuracisto Wang Yazhu</url></vrk>
          <lok>&Ccirc;ina Radio Internacia, 2018-07-31</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_j;&c_m;&c_a;&c_c;&c_mol; <klr>(&c_m;&c_je;&c_s;&c_c;&c_a; &c_c;&c_a;&c_l;&c_k;&c_a;&c_m;)</klr></trd>,
    <trd>&c_a;&c_h;&c_o;&c_p;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_p;&c_e;&c_w;&c_n;&c_u;&c_ju; &c_p;&c_l;&c_o;&c_sh;&c_ch;&c_u;)</klr></trd>
  </trdgrp>
  <trd lng="fr">couvrir<klr> (une surface donn&eacute;e)</klr></trd>
</drv>

