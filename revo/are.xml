<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>are</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="are.0o">
  <kap><tld/>o</kap>
  <snc mrk="are.0o.KOMUNE">
    <dif>
      Parto de la tera surfaco, kun difinitaj limoj a&ubreve; difinita uzo:
      <ekz>
        la inundo kovris milhektaran <tld/>on;
      </ekz>
      <ekz>
        alteri&gcirc;a <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="teren.0o">tereno</ref>,
      <ref cel="aren.0o">areno</ref>,
      <ref cel="region.0o">regiono</ref>.
    </refgrp>
  </snc>
  <snc mrk="are.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 29</lok></fnt>
    <dif>
      Mezuro de <ref cel="surfac.0o.MAT">surfaco</ref>:
      <ekz>
        la <tld/>o de <ref cel="angul.rekt0o">rektangulo</ref> egalas
        al la produto de du el &gcirc;iaj sinsekvaj lateroj;
      </ekz>
      <ekz>
        la <tld/>o de <ref cel="cirkl.0o.surfaco">cirklo</ref> estas
        proporcia al la <ref cel="kvadra1.0o.algebra">kvadrato</ref> de
        &gcirc;ia radiuso.
      </ekz>
    </dif>
    <trd lng="de">Fl&auml;cheninhalt</trd>
    <trd lng="en">area<klr tip="ind"> (of a surface)</klr></trd>
    <trd lng="ru">&c_p;&c_l;&c_o;&c_shch;&c_a;&c_d;&c_mol;<klr tip="ind"> (&c_p;&c_o;&c_v;&c_je;&c_r;&c_h;&c_n;&c_o;&c_s;&c_t;&c_i;)</klr></trd>
  </snc>
  <snc mrk="are.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      <ref tip="super" cel="grand.fizika0o">Fizika grando</ref>,
      esprimanta la mezuron de surfaco:
      <ekz>
        la <ref cel="dimens.0o.FIZ">dimensioj</ref> de <tld/>o estas
        <frm>M<sup>0</sup>L<sup>2</sup>T<sup>0</sup></frm>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="metr.kvadrata0o">kvadrata metro</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_l;&c_o;&c_sh;&c_ch;&c_a;</trd>, 
    <trd>&c_p;&c_r;&c_a;&c_s;&c_t;&c_o;&c_r;&c_a;</trd>, 
    <trd>&c_p;&c_a;&c_v;&c_je;&c_r;&c_h;&c_n;&c_ja;</trd>, 
    <trd>&c_z;&c_o;&c_n;&c_a;</trd>
  </trdgrp>  
  <trdgrp lng="fr">
    <trd>aire</trd>,
    <trd>superficie</trd>,
    <trd>surface<klr tip="ind"> (superficie)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_o;&c_shch;&c_a;&c_d;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_p;&c_o;&c_v;&c_je;&c_r;&c_h;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_o;&c_n;&c_a;</trd>,
    <trd>&c_u;&c_ch;&c_a;&c_s;&c_t;&c_o;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="are.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">MAT</uzo>
  <uzo tip="stl">ARK</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 25</lok></fnt>
  <snc>
    <dif>
      <ref tip="dif" cel="surfac.0o.MAT">Surfaco</ref>, precipe se
      &gcirc;i havas finian <tld/>on.
    </dif>
  </snc>
</drv>

<drv mrk="are.sam0a">
  <kap>sam<tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 25</lok></fnt>
  <snc>
    <dif>
      <klr>(p.p. <ref cel="surfac.0o.MAT">surfacoj</ref>)</klr>
      Havantaj saman <tld/>on.
    </dif>
  </snc>
</drv>

</art>
