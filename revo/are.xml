<drv mrk="are.0i">
  <kap>
    <tld/>i
  </kap>
  <snc mrk="are.0i.KOMUNE">
    <dif>
      <ref tip="vid" cel="amplek.0i">Ampleksi</ref>,
      plene <ref tip="vid" cel="okup.0i.lokigxi">okupi<sncref/></ref>:
      <ekz>
        la servejo <tld/>as proksimume 40 kvadratajn metrojn, estas orda kaj pura
        <fnt>
          <aut>&Gcirc;oja</aut>
          <vrk><url ref="&CRI;2521/2018/07/31/165s200237.htm">Unu tago de &gcirc;enerala kuracisto Wang Yazhu</url></vrk>
          <lok>&Ccirc;ina Radio Internacia, 2018-07-31</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_j;&c_m;&c_a;&c_c;&c_mol; <klr>(&c_m;&c_je;&c_s;&c_c;&c_a; &c_c;&c_a;&c_l;&c_k;&c_a;&c_m;)</klr></trd>,
    <trd>&c_a;&c_h;&c_o;&c_p;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_p;&c_e;&c_w;&c_n;&c_u;&c_ju; &c_p;&c_l;&c_o;&c_sh;&c_ch;&c_u;)</klr></trd>
  </trdgrp>
  <trd lng="fr">couvrir<klr> (une surface donn&eacute;e)</klr></trd>
</drv>

<drv mrk="are.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <fnt><bib>MatTerm</bib><lok>p. 25</lok></fnt>
  <snc mrk="are.0ajxo.MAT">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="dif" cel="surfac.0o.MAT">Surfaco</ref>, precipe se
      &gcirc;i havas finitan <tld/>on.
    </dif>
    <trdgrp lng="zh">
    <trd>&#x5206;&#x4F48;&#x5340; <pr>f&#x113;nb&#xF9;q&#x16B;</pr></trd>,
    <trd>&#x5206;&#x5E03;&#x533A; <pr>f&#x113;nb&#xF9;q&#x16B;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_p;&c_a;&c_v;&c_je;&c_r;&c_h;&c_n;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Fl&auml;che</trd>,
    <trd>Gebiet</trd>,
    <trd>Areal</trd>,
  </trdgrp>
  <trdgrp lng="en">
    <trd>area</trd>
  </trdgrp>
  <trd lng="he">&alef;&#x5B5;&zayin;&vav;&#x5B9;&resh;</trd>
  <trdgrp lng="nl">
    <trd>oppervlakte</trd>,
    <trd>gebied</trd>
  </trdgrp>
</drv>

<drv mrk="are.sam0a">
  <kap>sam<tld/>a</kap>
  <fnt><bib>MatTerm</bib><lok>p. 25</lok></fnt>
  <snc mrk="are.sam0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <klr>(pri <ref cel="surfac.0o.MAT">surfacoj</ref>)</klr>
      Havantaj saman <ref cel="are.0o"><tld/>on</ref>.
    </dif>
    <trdgrp lng="zh">
    <trd>&#x5E73;&#x9762;&#x76F8;&#x7B49; <pr>p&#xED;ngmi&#xE0;nxi&#x101;ngd&#x11B;ng</pr></trd>,
    <trd>&#x9762;&#x79EF;&#x76F8;&#x7B49; <pr>mi&#xE0;njixi&#x101;ngd&#x11B;ng</pr></trd>,
    <trd>&#x9762;&#x7A4D;&#x76F8;&#x7B49; <pr>mi&#xE0;njixi&#x101;ngd&#x11B;ng</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_d;&c_n;&c_o;&c_l;&c_mol;&c_k;&c_a;&c_v;&c_a;&c_j; <ind>&c_p;&c_l;&c_o;&c_sh;&c_ch;&c_y;</ind></trd>
  <trd lng="de">fl&auml;cheninhaltsgleich</trd>
  <trdgrp lng="en">
    <trd>of the same <ind>area</ind></trd>,
    <trd>same-<ind>area</ind></trd>
  </trdgrp>
  <trd lng="fr">de m&ecirc;me <ind>aire</ind></trd>
  <trd lng="hu">azonos ter&uuml;let&udblac;</trd>
  <trd lng="nl">met dezelfde <ind>oppervlakte</ind></trd>
  <trd lng="ru">&c_o;&c_d;&c_i;&c_n;&c_a;&c_k;&c_o;&c_v;&c_o;&c_j;
  <ind>&c_p;&c_l;&c_o;&c_shch;&c_a;&c_d;&c_i;</ind></trd>
