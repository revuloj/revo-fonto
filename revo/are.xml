  <trd lng="he">&alef;&#x5B5;&zayin;&vav;&#x5B9;&resh;</trd>
  <trd lng="hi">&#x915;&#x94D;&#x937;&#x947;&#x924;&#x94D;&#x930;</trd>
  <trd lng="hmn">cheeb tsam</trd>
  <trd lng="hr">podru&ccaron;je</trd>
  <trd lng="ht">z&ograve;n</trd>
  <trdgrp lng="hu">
    <trd>ter&uuml;let</trd>,
    <trd>fel&uuml;let<klr tip="ind"> (ter&uuml;letr&eacute;sz)</klr>
