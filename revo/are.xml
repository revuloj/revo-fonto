    <trdgrp lng="zh">
    <trd>&#24179;&#38754;&#30456;&#31561; <pr>p&iacute;ngmi&agrave;nxi&amacron;ngd&ecaron;ng</pr></trd>,
    <trd>&#38754;&#31215;&#30456;&#31561; <pr>mi&agrave;njixi&amacron;ngd&ecaron;ng</pr></trd>,
    <trd>&#38754;&#31309;&#30456;&#31561; <pr>mi&agrave;njixi&amacron;ngd&ecaron;ng</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_d;&c_n;&c_o;&c_l;&c_mol;&c_k;&c_a;&c_v;&c_a;&c_j; <ind>&c_p;&c_l;&c_o;&c_sh;&c_ch;&c_y;</ind></trd>
