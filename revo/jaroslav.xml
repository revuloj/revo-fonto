<art mrk="$Id: jaroslav.xml,v 1.5 2022/04/30 08:42:42 revo Exp $">
<kap>
  <rad>Jaroslav</rad>/o
</kap>

<drv mrk="jaroslav.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="jaroslav.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> slavdevena:
      <ekz>
        <tld/> longan tempon sonorigis &ccirc;e malalta pordo
        <fnt>
          <aut>H. Mal&iacute;&rcaron;ova, trad. V. Echtner</aut>,
          <vrk>La Ora Puto</vrk>,
          <lok>&Ccirc;e&hcirc;a kaj Slovaka Antologio, 1935</lok>
        </fnt>;
      </ekz>
      <ekz>
        post la morto de <tld/>o la Sa&gcirc;a en 1054 la regna
        konfederacio 
        de urbo&scirc;tatoj disrompi&gcirc;is
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Volinio">
          Volinio</url>, 2010-06-23</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_Ja;&c_r;&c_a;&c_s;&c_l;&c_a;&c_w;</trd>
    <trd lng="fr">Iaroslav</trd>
    <trdgrp lng="pl">
      <trd>Jaros&lstroke;aw</trd>,
      <trd>Jarek<klr> {diminutiv}</klr></trd>
  </trdgrp>
  </snc>
</drv>
</art>

