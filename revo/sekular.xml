<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>sekular</rad>/a
      <fnt><bib>PIV1</bib></fnt>
    </kap>

    <drv mrk="sekular.0a">
      <kap><tld/>a</kap>
      <snc>
        <dif>
          Ekstereklezia:
          <ekz>
            <tld/>a edukado;
          </ekz><ekz>
            <tld/>a tribunalo;
          </ekz><ekz>
            <tld/>a &scirc;tato;
          </ekz><ekz>
            <tld/>a geedzi&gcirc;o.
          </ekz>
        </dif>
      </snc>
      <ref tip="super" cel="civil.0a">civila</ref>
      <ref tip="vid" cel="laik.0a.neklerika">laika<sncref/></ref>
      <trdgrp lng="ru">
        <trd>&c_s;&c_v;&c_je;&c_t;&c_s;&c_k;&c_i;&c_j;<klr tip="ind">
(&c_n;&c_je; &c_c;&c_je;&c_r;&c_k;&c_o;&c_v;&c_n;&c_y;&c_j;)</klr></trd>,
        <trd>&c_s;&c_je;&c_k;&c_u;&c_l;&c_ja;&c_r;&c_n;&c_y;&c_j;</trd>
      </trdgrp>
    </drv>

    <drv mrk="sekular.0igi">
      <kap><tld/>igi</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Forpreni ion el la povo de instanco eklezia a&ubreve; religia kaj
          transdoni &gcirc;in al povo civila a&ubreve; &scirc;tata a&ubreve;
privata:
          <ekz>
            <tld/>igi la lernejojn;
          </ekz><ekz>
            <tld/>igi la mona&hcirc;ejajn bienojn;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            <tld/>igi la literaturon, la filozofion
            <klr>(liberigi ilin el la religiaj katenoj)</klr>
          </ekz>
        </dif>
      </snc>
      <trd
lng="be">&c_s;&c_e;&c_k;&c_u;&c_l;&c_ja;&c_r;&c_y;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
      <trd lng="de">s&auml;kularisieren</trd>
      <trdgrp lng="hu">
        <trd>szekulariz&aacute;l</trd>,
        <trd>elvil&aacute;gias&iacute;t</trd>
      </trdgrp>
      <trd lng="nl">seculariseren</trd>
      <trd
lng="ru">&c_s;&c_je;&c_k;&c_u;&c_l;&c_ja;&c_r;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </drv>
  </art>


<!--
$Log$
-->
</vortaro>