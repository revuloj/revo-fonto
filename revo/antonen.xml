<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: antonen.xml,v 1.5 2021/05/10 18:13:57 revo Exp $">
<kap>
  <rad>Antonen</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="antonen.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref>
      latindevena:
      <ekz>
        la prioroj de la levensa paro&hcirc;o de Sankta <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=149070">Levens</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    Ekzistas anka&ubreve; similsona virina nomo <ref tip="hom" cel="anton.0ino">Antonino</ref>.
  </rim>
  <trdgrp lng="de">
    <trd>Antonin</trd>,
    <trd>Antoni</trd>,
    <trd>Antoninus</trd>
  </trdgrp>
  <trd lng="fr">Antonin</trd>
  <trd lng="ru">&c_A;&c_n;&c_t;&c_o;&c_n;&c_i;&c_n;</trd>
</drv>

<drv mrk="antonen.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="antonen.0oj.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Genra nomo de la <ref tip="super" cel="dinast.0o">dinastio</ref>
      de 7 imperiestroj de Romio, kiuj regis ekde la jaro 96&#170; p.K. &gcirc;is la 192&#170;.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Adoptivkaiser</trd>,
    <trd>Antoninische Dynastie</trd>
  </trdgrp>
  <trd lng="fr">Antonins</trd>
  <trd lng="ru">&c_A;&c_n;&c_t;&c_o;&c_n;&c_i;&c_n;&c_y;</trd>
</drv>

</art>

