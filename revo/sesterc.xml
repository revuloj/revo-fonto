<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sesterc.xml,v 1.10 2023/10/07 17:24:13 revo Exp $">
<kap>
  <rad>sesterc</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sesterc.0o">
  <kap><tld/>o</kap>
  <snc mrk="sesterc.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Romia <ref tip="lst" cel="unu.monasub0o.EKON" lst="voko:monunuoj">mona
      subunuo</ref>, kvarono de denaro:
      <ekz>
        li respondis, ke dum la tuta vivo li &scirc;paris <tld/>on post
        <tld/>o, por ela&ccirc;eti sian amatan filon
        <fnt><aut>H. Sienkewicz, trad. L. Zamenhof</aut>, <vrk>Quo
        vadis?</vrk>, <lok>1933</lok></fnt>;
      </ekz>
      <ekz>
        tri mil <tld/>ojn tio kostas por junulino de via a&gcirc;o
        <fnt><aut>A. L&ouml;wenstein</aut>, <vrk>La &scirc;tona urbo</vrk>,
        <lok>FEL, 1999</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_s;&c_t;&c_e;&c_r;&c_c;&c_y;&c_ja;</trd>
  <trd lng="cs">sesterce</trd>
  <trd lng="de">Sesterz</trd>
  <trdgrp lng="en">
    <trd>sestertius</trd>,
    <trd>sesterce</trd>
  </trdgrp>
  <trd lng="fr">sesterce</trd>
  <trd lng="it">sesterzo</trd>
  <trd lng="pl">sestercja</trd>
  <trd lng="sk">sestercia</trd>
  <trd lng="uk">&c_s;&c_je;&c_s;&c_t;&c_je;&c_r;&c_c;&c_ib;&c_j; (&c_d;&c_a;&c_v;&#x43D;&#x44C;&#x43E;&c_r;&c_i;&#x43C;&c_s;&#x44C;&#x43A;&c_a;
  &#x43C;&#x43E;&#x43D;&c_je;&c_t;&c_a;)</trd>
</drv>
</art>

