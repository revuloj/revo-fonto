<drv mrk="tem.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="tem.pri0i.KOMUNE">
    <dif>
      <ref tip="vid" cel="koncer.0i">Koncerni</ref>, havi kiel
      <tld/>on:
      <ekz>
        romano, kiu pri<tld/>as ekskluzive la vivon de E-klubo
        <fnt>
          <aut>UEA</aut>,
          <vrk>Katalogo</vrk>,
          <lok>[vidita en 2008]</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vorto &leftquot;intima&#x201D; <klr>[&#8230;]</klr> ne nur <klr>[&#8230;]</klr>
        priskribas a&ubreve; pri<tld/>as erotikon
        <fnt>
          <bib>Monato</bib>
          <aut>Reinaldo Marcelo Ferreira</aut>
          <vrk><url ref="&Monato;/2005/008629.php">Ne nur intimaj
          temoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kvankam &ccirc;efe biografio, &ccirc;i tiu libro anka&ubreve; detale klarigas
        <klr>[&#8230;]</klr>
        mondajn eventojn de la pri<tld/>ata periodo
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2006/008886.php">Prisilentita
          heroo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la enhavo de &ccirc;i tiu verko pri<tld/>as la vivon en Transilvanio
        dum tiu periodo
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2008/009584.php">Kvaza&ubreve;a&ubreve;tobiografia
          rakonto tre maltrankviliga</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">um etwas <ind>gehen</ind></trd>
  </snc>
  <trd lng="be">&c_t;&c_y;&c_ch;&c_y;&c_c;&c_c;&c_a; <klr>(&c_n;&c_je;&c_j;&c_k;&c_a;&c_j; &c_t;&c_e;&c_m;&c_y;)</klr></trd>
  <trdgrp lng="fr">
    <trd>concerner</trd>,
    <trd>traiter<klr> (de)</klr></trd>
  </trdgrp>
  <trd lng="id">ber<ind>tema</ind>kan</trd>
  <trd lng="nl">betreffen</trd>
</drv>

<drv mrk="tem.0o_kun_variajxoj">
  <kap><tld/>o kun varia&jcirc;oj<fnt><bib>MuzTerm</bib></fnt>
    <var><kap>varia&jcirc;oj pri <tld/>o</kap></var>
  </kap>
  <snc mrk="tem.0o_kun_variajxoj.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="kompon.0ajxo">Kompona&jcirc;o</ref>
      el melodio, tielnomata <tld/>o, kaj
      <ref tip="vid" cel="vari.0ajxo">varia&jcirc;oj</ref>
      konservantaj la skemon de la <tld/>o sed variigantaj &gcirc;in
      diversmaniere:
      <ekz>
        <nom>Jean Sibelius</nom> &#8208; <tld lit="T"/>o kaj Varia&jcirc;oj por Sola
        Violon&ccirc;elo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=12984">Jean
          Sibelius</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la iel popularaj Simfoniaj varia&jcirc;oj pri <tld/>oj de <nom>Carl
        Maria von Weber</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2014/011202.php">Pura muziko por
          eonoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Thema und Variationen</trd>,
      <trd>Variationen &uuml;ber ein Thema</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_m;&c_a; &c_z; &c_v;&c_a;&c_r;&c_y;&c_ja;&c_c;&c_y;&c_ja;&c_m;&c_ib;</trd>
  <trd lng="fr">th&egrave;me et variations</trd>
  <trd lng="hu">t&eacute;ma &eacute;s vari&aacute;ci&oacute;k</trd>
  <trd lng="id"><ind>tema</ind> dan variasi</trd>
  <trd lng="pl">temat z wariacjami</trd>
  <trd lng="ru">&c_t;&c_je;&c_m;&c_a; &c_s;
  &c_v;&c_a;&c_r;&c_i;&c_a;&c_c;&c_i;&c_ja;&c_m;&c_i;</trd>
</drv>
