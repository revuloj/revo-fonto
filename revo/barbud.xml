<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: barbud.xml,v 1.6 2018/09/10 07:10:14 revo Exp $">
<kap>
  <rad>barbud</rad>/o
</kap>

<drv mrk="barbud.0o">
  <kap>
    <tld lit="B"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="barbud.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" lst="voko:insuloj" cel="insul.0o">Insulo</ref>
      en la Centra <ref tip="malprt" cel="amerik.A0o">Ameriko</ref>
      en Kariba Maro, en insularo Deventaj Insuloj,
      apartenanta al 
      <ref tip="vid" cel="antigv.0o.sxtato">Antigvo kaj Barbudo</ref>:
      <ekz>
        la <tld lit="B"/>a parulio (<nom>Dendroica subita</nom>) estas malgranda paserina 
        birdo <klr>[&#8230;]</klr>, endemia de la insulo de <tld lit="B"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=326955">Barbuda parulio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    </snc>
    <trd lng="bg">&c_B;&c_a;&c_r;&c_b;&c_u;&c_d;&c_a;</trd>
    <trd lng="de">Barbuda</trd>
    <trd lng="fr">Barbuda</trd>
    <trd lng="id">Barbuda</trd>
    <trdgrp lng="pl">
      <trd>Barbuda</trd>
    </trdgrp>
</drv>

</art>

