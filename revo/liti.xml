<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>liti</rad>/o
</kap>
<drv mrk="liti.0o">
  <kap>
    <tld/>o
  </kap>
  <mlg kod="IUPAC">Li</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
      lst="voko:&hcirc;emiaj_elementoj" val="3">elemento</ref> kun
      atomnumero 3, atompezo 6,939;
      <ref tip="super" cel="alkal.0eca">alkala</ref> 
      <ref tip="super" cel="metal.0o.KEM">metalo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_t;&c_y;&c_j;</trd>
  <trd lng="bg">&c_l;&c_i;&c_t;&c_i;&c_j;</trd>
  <trd lng="br">litiom</trd>
  <trd lng="de">Lithium</trd>
  <trd lng="el">&lambda;&jota_ton;&theta;&jota;&omikron;</trd>
  <trd lng="en">lithium</trd>
  <trd lng="es">litio</trd>
  <trd lng="fa">&a_l;&f_y;&a_t;&f_y;&a_m;</trd>
  <trd lng="fr">lithium</trd>
  <trd lng="hu">l&iacute;tium</trd>
  <trd lng="it">litio</trd>
  <trd lng="la">Lithium</trd>
  <trd lng="nl">lithium</trd>
  <trd lng="pl">lit</trd>
  <trd lng="pt">l&iacute;tio</trd>
  <trd lng="ru">&c_l;&c_i;&c_t;&c_i;&c_j;</trd>
  <trd lng="sv">litium</trd>
  <trd lng="tr">lityum</trd>
</drv>
</art>
