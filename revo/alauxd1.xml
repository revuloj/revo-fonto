<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: alauxd1.xml,v 1.11 2023/05/11 09:25:20 revo Exp $">
<kap>
  <rad>ala&ubreve;ded</rad>/oj
</kap>
<drv mrk="alauxd1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="paser.0oformaj">paseroformaj</ref> birdoj
      (<trd lng="de">Alaudidae</trd>):
      <ekz>
        <tld/>oj estas familio de paserinaj birdoj kun <klr>[&#8230;]</klr> 100 specioj
        <klr>[&#8230;]</klr>, kiuj lo&gcirc;as en la Malnova Mondo, sed inkludante nordan kaj
        orientan A&ubreve;stralion, nur unu, la Montala&ubreve;do, disvasti&gcirc;is al Nordameriko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18179">Ala&ubreve;dedoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="alauxd.0o">ala&ubreve;do</ref>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_v;&c_a;&c_r;&c_a;&c_n;&c_k;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">al&agrave;udids</trd>
  <trd lng="de">Lerchen</trd>
  <trdgrp lng="en">
     <trd>passeriformes</trd>,
     <trd>larks</trd>,
     <trd>Alaudidae</trd>
  </trdgrp>
  <trd lng="fr">alaudid&eacute;s</trd>
  <trd lng="hu">pacsirtaf&eacute;l&eacute;k</trd>
  <trd lng="pl">skowronki</trd>
  <trd lng="pt">alaud&iacute;deos</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_a;&c_v;&c_o;&c_r;&c_o;&c_n;&c_k;&c_o;&c_v;&c_y;&c_je;</trd>,
    <trd>&c_zh;&c_a;&c_v;&c_o;&c_r;&c_o;&c_n;&c_k;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="uk">&c_zh;&c_a;&c_j;&c_v;&#x43E;&c_r;&#x43E;&#x43D;&#x43A;&#x43E;&c_v;&c_ib;</trd>
</drv>
</art>

