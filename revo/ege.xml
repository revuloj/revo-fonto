      <ofc>10</ofc><rad>Ege</rad>/o
    </kap>

<drv mrk="ege.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MIT</uzo>
    <dif>
      Mita re&gcirc;o de Atiko:
      <ekz>
        <tld/>o estis <klr>[&#8230;la]</klr> patro de Tezeo, kiu mortigis la Minota&ubreve;ron
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=54357"
              >Egeo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="mar.Egea_0o">Egea Maro</ref>
    <trd lng="be">&c_E;&c_g;&c_je;&c_j;</trd>
    <trdgrp lng="de">
      <trd>Aigeus</trd>,
      <trd>&Auml;geus</trd>
    </trdgrp>
    <trd lng="en">Aegeus</trd>
    <trd lng="fr">&Eacute;g&eacute;e</trd>
    <trd lng="grc">&Alfa;&jota_psili;&gamma;&epsilon;&ypsilon_ton;&sigma_fina;</trd>
    <trd lng="pl">Aegaeon</trd>
  </snc>
 </drv>
</art>

