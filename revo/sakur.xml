<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sakur.xml,v 1.17 2023/09/03 12:09:43 revo Exp $">
<kap>
  <rad>sakur</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="sakur.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Specioj kaj sub-specioj de
      <ref tip="super" cel="prunus.0o">prunuso</ref>,
      similaj al &ccirc;erizarboj, famaj precipe en Japanio
      pro belega kaj efemera helru&gcirc;a florado:
      <ekz>
        en &Ccirc;inio plej populara arbo por florrigardado estas umeo (t.n.
        &ccirc;ina 
        pruno, fakte specio de abrikoto), sed en Japanio <tld/>oj prenis
        tiun 
        pozicion almena&ubreve; ekde la mezo de 9-a jarcento
        <fnt><bib>Viki</bib>, <lok>Sakuro</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_a;&c_k;&c_u;&c_r;&c_a;</trd>
    <trdgrp lng="de">
      <trd>japanische <ind>Kirsche</ind></trd>,
      <trd>Sakura-Kirsche</trd>
    </trdgrp>
    <trd lng="en">cherry blossom</trd>
    <trdgrp lng="fr">
      <trd>cerisier-fleur</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>szakura</trd>,
      <trd>vadmeggy<klr tip="amb"> (jap&aacute;n)</klr></trd>
    </trdgrp>
    <trd lng="nl">Japanse <ind>kerselaar</ind></trd>
    <trd lng="pl">sakura</trd>
    <trd lng="sk">sakura</trd>
    <trd lng="ru">&c_s;&c_a;&c_k;&c_u;&c_r;&c_a;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>japonsk&#xE1; t&rcaron;e&scaron;e&ncaron;</trd>,
    <trd>sakura</trd>
  </trdgrp>
  <trd lng="ja">&#x30B5;&#x30AF;&#x30E9;</trd>
  <trdgrp lng="sk">
    <trd>japonsk&#xE1; &#x10D;ere&scaron;&ncaron;a</trd>,
    <trd>sakura, japonsk&#xE1; &#x10D;ere&scaron;&ncaron;a</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_a;&#x43A;&c_u;&c_r;&c_a;</trd>
</drv>
</art>
