<drv mrk="delika.mal0e">
  <kap>mal<tld/>e</kap>
  <snc mrk="delika.mal0e.KOMUNE">
    <dif>
      Krude:
      <ekz>
        li mal<tld/>e tiris min anta&ubreve;en;
      </ekz>
      <ekz>
        vi regas super ili mal<tld/>e kaj kruele
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 34:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne konvenas tro mal<tld/>e forpu&scirc;i virinon, kiu amis vin
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fratino mal<tld/>e de&scirc;iris la virgan vualon, kovranta la revojn de &scirc;ia koro
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Volap&uuml;k</nom> sonas tre sova&gcirc;e kaj mal<tld/>e, Esperanto estas plena je harmonio kaj
        estetiko
        <fnt>
          <bib>EE</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kares.mal0i">malkaresi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_r;&c_u;&c_b;&c_a;</trd>,
    <trd>&c_n;&c_je;&c_d;&c_a;&c_l;&c_ib;&c_k;&c_a;&c_t;&c_n;&c_a;</trd>,
    <trd>&c_v;&c_u;&c_l;&c_mol;&c_g;&c_a;&c_r;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
