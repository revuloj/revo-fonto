<drv mrk="delika.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="delika.0ajxo.KOMUNE">
    <dif>
      <tld lit="D"/>a objekto:
      <ekz>
        ili dormadis sur molaj kusenoj <klr>[&#x2026;]</klr>, man&gcirc;adis bonegajn <tld/>a&jcirc;ojn
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a&jcirc;o por mo&scirc;tuloj
        <fnt>
          <aut>G. G&aacute;rdonyi, trad. J. Horvath</aut>
          <vrk><url ref="http://vortaro.hu/stelspec.pdf">
          Steloj de Eger</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili tiel pruvas, pli kaj pli, la mirindan kapablon de Esperanto esprimi tute la
        <tld/>a&jcirc;ojn de la spirito kaj de la koro
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">bibelot</trd>
</drv>


<drv mrk="delika.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="delika.mal0a.io">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Forte kaj akre impresanta,
      <ref tip="vid" cel="krud.0a.FIG">kruda<sncref/></ref>:
      <ekz>
        mal<tld/>a laboro
        <fnt>
          <bib>Fab2</bib>
          <lok>Flikilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun granda, mal<tld/>a viza&gcirc;o, kvaza&ubreve;
        elhakita per hakilo, &scirc;i timigis min
          <fnt>
            <aut>M. Gorkij</aut>
            <vrk>Bol&ccirc;jo</vrk>
            <lok>en: Fundamenta Krestomatio
            de la lingvo Esperanto, 1904</lok>
          </fnt>.
      </ekz>
    </dif>
    <subsnc mrk="delika.mal0a.io_malbona">
      <dif>
        Malpla&ccirc;a per sia malaltkvalito kaj vulgareco, kruda:
        <ekz>
          neniam anta&ubreve;e li estis en tia mal<tld/>a kaj needukita kompanio
          <fnt>
            <bib>Fab1</bib>
            <lok>Galo&scirc;oj de feli&ccirc;o</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="delika.mal0a.io_bona">
      <dif>
        Fortika, malfacile difektebla:
        <ekz>
          oni vestis &scirc;in per kitelo el mal<tld/>a saktolo
          <fnt>
            <bib>Fab1</bib>
            <lok>Sova&gcirc;aj cignoj</lok>
          </fnt>;
        </ekz>
        <ekz>
          unuj portis longajn vestojn el mal<tld/>a drapo
          <fnt>
            <bib>Far2</bib>
            <lok>&ccirc;apitro 7a</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
  </snc>
  <snc mrk="delika.mal0a.iu">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Malakresenta, malsubtila, ne facile impresebla.
    </dif>
    <subsnc mrk="delika.mal0a.iu_malbona">
      <dif>
        Sentakta, nediskreta, troigema, ofendema:
        <ekz>
          mi devas esti mal<tld/>a kaj severa, se mi volas teni en ordo miajn filojn
          <fnt>
            <bib>Fab1</bib>
            <lok>&Gcirc;ardeno de la paradizo</lok>
          </fnt>;
        </ekz>
        <ekz>
          mal<tld/>aj krea&jcirc;oj, ili fortran&ccirc;is pecon de mia belega barbo
          <fnt>
            <bib>ElektFab</bib>
            <lok>Ne&gcirc;ulino kaj Rozulino</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
  </snc>
  <trdgrp lng="ca">
    <trd>bast</trd>,
    <trd>grosser</trd>,
    <trd>tosc</trd>
  </trdgrp>
  <trd lng="cs">hrub&yacute;</trd>
  <trdgrp lng="en">
    <trd>vulgar</trd>,
    <trd>crude</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fruste</trd>,
    <trd>grossier<klr tip="amb"> (sans d&eacute;licatesse)</klr></trd>,
    <trd>robuste</trd>,
    <trd>costaud</trd>,
    <trd>ind&eacute;licat</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&gimel;&samekh;</trd>,
    <trd>&lamed;&alef; <ind>&tsadi;&ayin;&vav;&dalet;&fnun;</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nyers</trd>,
    <trd>durva</trd>,
    <trd>er&odblac;s</trd>,
    <trd>darabos</trd>,
    <trd>k&ouml;z&ouml;ns&eacute;ges</trd>,
    <trd>tapintatlan</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>niedelikatny</trd>,
    <trd>grubia&nacute;ski</trd>,
    <trd>opryskliwy</trd>,
    <trd>wulgarny</trd>
  </trdgrp>
  <trd lng="ro">brutal</trd>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_b;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>grobiansky</trd>,
    <trd>hrub&yacute;</trd>,
    <trd>neslu&scaron;n&yacute;</trd>,
    <trd>surov&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="delika.mal0e">
  <kap>mal<tld/>e</kap>
  <snc mrk="delika.mal0e.KOMUNE">
    <dif>
      Krude:
      <ekz>
        li mal<tld/>e tiris min anta&ubreve;en;
      </ekz>
      <ekz>
        vi regas super ili mal<tld/>e kaj kruele
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 34:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne konvenas tro mal<tld/>e forpu&scirc;i virinon, kiu amis vin
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fratino mal<tld/>e de&scirc;iris la virgan vualon, kovranta la revojn de &scirc;ia koro
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Volap&uuml;k</nom> sonas tre sova&gcirc;e kaj mal<tld/>e, Esperanto estas plena je harmonio kaj
        estetiko
        <fnt>
          <bib>EE</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kares.mal0i">malkaresi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>bastament</trd>,
    <trd>ordin&agrave;riament</trd>
  </trdgrp>
  <trd lng="cs">hrub&ecaron;</trd>
  <trd lng="en">crudely</trd>
  <trdgrp lng="fr">
    <trd>cr&ucirc;ment</trd>,
    <trd>rudement</trd>,
    <trd>brusquement<klr tip="ind"> (sans &eacute;gards)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>niedelikatnie</trd>,
    <trd>grubia&nacute;sko</trd>,
    <trd>opryskliwie</trd>,
    <trd>wulgarnie</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_b;&c_o;</trd>
  <trd lng="sk">hrubo</trd>
</drv>

<drv mrk="delika.mal0ulo">
  <kap>mal<tld/>ulo</kap>
  <snc mrk="delika.mal0ulo.KOMUNE">
    <dif>
      <ref cel="krud.0ulo">Krudulo</ref>,
      <ref cel="brut.0o.FIG">bruto<sncref/></ref>:
      <ekz>
        ni ne englutas tiel multe kiel vi, avidaj mal<tld/>uloj
        <fnt>
          <bib>ElektFab</bib>,
          <vrk>Ne&gcirc;ulino kaj Rozulino</vrk>
        </fnt>;
      </ekz>
      <ekz>
        tio estas mal<tld/>ulo, la tutan vivon li persekutas barbarojn, oni do povas
        pardoni al li
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi tute ne atendis, ke li estos tia mal<tld/>ulo! prezentu al vi, kiam mi
        komencis mian ripro&ccirc;an parolon, li tuj volis doni al mi du survangojn
        <fnt>
          <bib>FK</bib>
        </fnt>!
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxtip.0o">&scirc;tipo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>barroer</trd>,
    <trd>bast<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="cs">hrubec</trd>
  <trdgrp lng="en">
    <trd><ind>crude</ind> person</trd>,
    <trd><ind>vulgar</ind> person</trd>
  </trdgrp>
  <trd lng="fr">rustre</trd>
  <trd lng="he">&alef;&dalet;&fmem; <ind>&gimel;&samekh;</ind></trd>
  <trd lng="pl">grubianin</trd>
  <trdgrp lng="sk">
    <trd>grobian</trd>,
    <trd>hrub&yacute; &ccaron;lovek</trd>,
    <trd>neslu&scaron;n&yacute;</trd>,
    <trd>surovec</trd>
  </trdgrp>
</drv>

