<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: jung.xml,v 1.34 2024/08/16 21:24:35 revo Exp $">
<kap>
  <ofc>*</ofc> <rad>jung</rad>/i <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="jung.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="jung.0i.RAJ">
    <dif>
      <ref tip="super" cel="lig.al0i">Alligi</ref> <ref cel="best.tir0o">tirbesto</ref>n
      a&ubreve; motoron al la tirota objekto:
      <ekz>
        <tld/>u la bovinojn al la &ccirc;aro
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 6:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; la pordo jam staris kale&scirc;o, al kiu estis <tld/>itaj ok blankaj &ccirc;evaloj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Re&gcirc;o rano a&ubreve; fera Henriko</lok>
        </fnt>;
      </ekz>
      <ekz>
        li &scirc;ar&gcirc;is &ccirc;aron per ligno, <tld/>is du bovojn kaj veturis en la urbon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Doktoro &Ccirc;ioscia</lok>
        </fnt>;
      </ekz>
      <ekz>
        la virinoj kaj infanoj devis sin <tld/>i al la plugilo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne <tld/>u la bovon malanta&ubreve; la plugilon
        <fnt><bib>PV</bib></fnt>;
      </ekz><ekz>
        <tld/>i lokomotivon al vagonaro;
      </ekz><ekz>
        kiu sin en<tld/>is, devas tiri
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        novaj ideoj <tld/>itaj al malnovaj teorioj
        <fnt>
          <bib>Monato</bib>
          <aut>Dmitrio Lytov</aut>
          <vrk><url ref="&Monato;/2003/008175.html">Novaj ideoj jungitaj al malnovaj
          teorioj</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="jung.0i.movaton">
    <dif>
      <ref cel="jung.pri0i">Pri<tld/>i</ref>:
      <ekz>
        <nom>Jozef</nom> <tld/>is sian &ccirc;aron kaj veturis renkonte al sia patro
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 46:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        la markizo <klr>[&#8230;]</klr> tuj <tld/>igis sian kale&scirc;on
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la le&ubreve;tenanto Karadelo tute ne scias la naturon de ligiloj, kiuj min <tld/>as
        kun Frederiko
        <fnt>
          <bib>KPr</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_p;&c_r;&c_a;&c_g;&c_a;&c_c;&c_mol;</trd>
  <trd lng="br">sternia&ntilde;<klr tip="ind"> (loened-labour)</klr></trd>
  <trd lng="cs">zap&rcaron;&aacute;hnout</trd>
  <trdgrp lng="de">
    <trd>einspannen</trd>,
    <trd>anschirren</trd>,
    <trd>vorspannen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>harness</trd>,
    <trd>yoke</trd>
  </trdgrp>
  <trd lng="es">aparejar (animales de tiro o carga)</trd>
  <trd lng="fr">atteler</trd>
  <trdgrp lng="hu">
    <trd>befog <klr tip="ind">(kocsiba)</klr></trd>,
    <trd>felszersz&aacute;moz <klr tip="ind">(igavon&oacute;t)</klr></trd>,
    <trd>felkant&aacute;roz</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12388;&#12394;&#12368;</trd>,
    <trd>&#32283;&#12426;&#12388;&#12369;&#12427;  <pr>&#12375;&#12400;&#12426;&#12388;&#12369;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">inspannen<klr tip="ind"> (v.dier)</klr></trd>
  <trdgrp lng="pt">
    <trd>atrelar</trd>,
    <trd>jungir</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_p;&c_r;&c_ja;&c_g;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_r;&c_ja;&c_ch;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zapriahnu&tcaron;</trd>
  <trd lng="uk">&c_z;&c_a;&c_p;&c_r;&c_ja;&c_g;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#29301;&#32458; <pr>qi&amacron;nb&agrave;n</pr></trd>,
    <trd>&#22871;&#39340; <pr>t&agrave;om&#462;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="jung.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc mrk="jung.0ajxo.ilaro">
    <dif>
      <ref tip="dif" cel="jung.0ilaro"><tld lit="J"/>ilaro</ref>:
      <ekz>
        sur la <tld/>a&jcirc;o de la bovoj li kuiris la viandon
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 19:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a&jcirc;o difekti&gcirc;is &ccirc;iuminute, la laboroj estis plenumataj malzorge
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_b;&c_r;&c_u;&c_ja;</trd>,
      <trd>&c_v;&c_u;&c_p;&c_r;&c_a;&c_zh;</trd>
    </trdgrp>
    <trd lng="de">Geschirr</trd>
    <trd lng="zh">&#25405;&#20855; <pr>w&#462;nj&ugrave;</pr></trd>
</snc>
  <snc mrk="jung.0ajxo.jungitaro">
    <dif>
      <ref tip="dif" cel="jung.0itaro"><tld lit="J"/>itaro</ref>:
      <ekz>
        Mi frakasis per vi pa&scirc;tiston kaj lian &scirc;afaron, Mi frakasis per vi terlaboriston
        kaj lian <tld/>a&jcirc;on
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 51:23</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_a;&c_p;&c_r;&c_e;&c_zh;&c_k;&c_a;</trd>
    <trd lng="de">Gespann</trd>
  </snc>
  <trd lng="uk">&c_u;&c_p;&c_r;&c_ja;&c_zh;</trd>
</drv>

<drv mrk="jung.0ilaro">
  <kap><tld/>ilaro, <var><kap><tld/>iloj</kap></var></kap>
  <snc mrk="jung.0ilaro.RAJ">
    <dif>
      Aro de la rimenoj, &scirc;nuroj kaj aliaj uzataj por <tld/>i;
      <ref tip="sin" cel="harnis.0o">harniso</ref>:
      <ekz>
        la dra&scirc;iloj kaj la <tld/>ilaro de la bovoj servos kiel ligno
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 24:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Don</nom> Ki&hcirc;oto eliris promeni sur la marbordo, armite per &ccirc;iuj defendiloj, &ccirc;ar, kiel
        li ofte diris, ili estas liaj <tld/>iloj, kaj la batalado estas lia ripozo
        <fnt>
          <bib>DKM</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;evalskeletoj, havantaj <tld/>ilaron el ru&gcirc;aj fajraraneoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
       <ref cel="jug.0o">jugo</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_z;&c_b;&c_r;&c_u;&c_ja;</trd>,
      <trd>&c_v;&c_u;&c_p;&c_r;&c_a;&c_zh;</trd>
    </trdgrp>
    <trd lng="br">stern<klr tip="ind"> (loened-labour)</klr></trd>
    <trd lng="ca">arn&eacute;s</trd>
    <trdgrp lng="de">
      <trd>Zaumzeug</trd>,
      <trd>Pferdegeschirr</trd>,
      <trd>Geschirre</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>harness</trd>,
      <trd>yoke</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>arn&eacute;s</trd>
      <trd>aparejos (de montura o tiro)</trd>,
      <trd>guarnici&oacute;n (de montura o tiro)</trd>
    </trdgrp>
    <trd lng="fa">&a_s;&a_A;&a_z; &a_w; &a_b;&a_r;&f_g;</trd>
    <trd lng="fr">harnais</trd>
    <trdgrp lng="hu">
      <trd>h&aacute;m</trd>,
      <trd>l&oacute;szersz&aacute;m</trd>
    </trdgrp>
    <trd lng="it">finimenti</trd>
    <trdgrp lng="nl">
      <trd>tuig</trd>,
      <trd>span</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>arn&ecirc;s</trd>,
      <trd>arreios</trd>,
      <trd>arreio</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_u;&c_p;&c_r;&c_ja;&c_zh;&c_mol;</trd>,
      <trd>&c_s;&c_b;&c_r;&c_u;&c_ja;</trd>
    </trdgrp>
    <trd lng="zh">&#25405;&#20855; <pr>w&#462;nj&ugrave;</pr></trd>
</snc>
  <trd lng="cs">postroj</trd>
  <trdgrp lng="ja">
    <trd>&#39340;&#20855;  <pr>&#12400;&#12368;</pr></trd>,
    <trd>&#24341;&#12365;&#20855;  <pr>&#12402;&#12365;&#12368;</pr></trd>
  </trdgrp>
  <trd lng="sk">postroj</trd>
  <trd lng="uk">&c_u;&c_p;&c_r;&c_ja;&c_zh;</trd>
</drv>

<drv mrk="jung.0itaro">
  <kap><tld/>itaro</kap>
  <snc mrk="jung.0itaro.RAJ">
    <dif>
      <tld lit="J"/>itaj bestoj:
      <ekz>
        li estis atake batita de fu&gcirc;inta jungitaro kaj <klr>[&#8230;]</klr> lia kruro estis
        rompita en du lokoj
        <fnt>
          <bib>LOdE</bib>
          <aut>Mark Twain</aut>
          <vrk><url ref="&Tekstaro;?nomo=ondo-de-esperanto">Malfacilas esti
          kandidato</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Gespann</trd>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_p;&c_r;&c_e;&c_zh;&c_k;&c_a;</trd>
  <trd lng="br">tenn<klr tip="ind"> (b.)</klr></trd>
  <trdgrp lng="cs">
    <trd>potah</trd>,
    <trd>sp&rcaron;e&zcaron;en&iacute;</trd>
  </trdgrp>
  <trd lng="en">team</trd>
  <trd lng="fr">attelage<klr tip="ind"> (animaux attel&eacute;s)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#19968;&#32068;&#12398;&#21205;&#29289; <pr>&#12402;&#12392;&#12367;&#12415;&#12398;&#12393;&#12358;&#12406;&#12388;</pr></trd>,
    <trd>&#36899;&#30044; <pr>&#12428;&#12435;&#12385;&#12367;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_p;&c_r;&c_ja;&c_zh;&c_k;&c_a;</trd>
  <trd lng="sk">z&aacute;prah</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_p;&c_r;&c_ja;&c_g;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_r;&c_ja;&c_zh;&c_k;&c_a;</trd>,
    <trd>&c_u;&c_p;&c_r;&c_ja;&c_zh;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#22242;&#38431; <pr>tu&aacute;ndu&igrave;</pr></trd>,
    <trd>&#22242;&#20307; <pr>tu&aacute;nt&#464;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="jung.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="jung.mal0i.RAJ">
    <dif>
      Liberigi tirbeston el <tld/>ado:
      <ekz>
        mal<tld/>u tuj la &ccirc;evalojn kaj suprenigu ilin per &scirc;nuro
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>,
        <lok>&ccirc;apitro 21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_p;&c_r;&c_a;&c_g;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">vyp&rcaron;&aacute;hnout</trd>
  <trd lng="de">ausspannen</trd>
  <trdgrp lng="en">
    <trd>unharness</trd>,
    <trd>unyoke</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;teler</trd>
  <trd lng="ja">&#12399;&#12378;&#12377;</trd>
  <trd lng="nl">uitspannen</trd>
  <trd lng="sk">vypriahnu&tcaron;</trd>
  <trd lng="uk">&c_r;&c_o;&c_z;&c_p;&c_r;&c_ja;&c_g;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="jung.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="jung.pri0i.provizi">
    <dif>
      Provizi veturilon a&ubreve; alian tirotan objekton je tirbesto a&ubreve; alia
      movilo:
      <ekz>
        oni <klr>[&#8230;]</klr> metis &scirc;in en la postan parton de la akjo,
        pri<tld/>ita per unu el la pezaj virbestoj de la patro
        <fnt><aut>Einar Walquist</aut>:
          <vrk>Kristina. Rakonto el sveda Laponujo</vrk>.
          <lok>Literatura Mondo, 1936:6, p. 154</lok>
        </fnt>;
      </ekz><ekz>
        atendante dek minutojn dum kiuj lia ko&ccirc;ero pri<tld/>is kaj
        preparis la veturilon
        <fnt>
          <aut>Marselo Prust, tr. &Jcirc;eromo Va&scirc;e</aut>:
          <vrk><url ref="http://jxvasxe.free.fr/tradukoj/la-vojo-de-svan-eo.htm">Sur la vojo de
          Svan</url></vrk>
          <lok>Parto 2&quadrat;: Unu amo de Svan</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_p;&c_r;&c_a;&c_g;&c_a;&c_c;&c_mol; <klr>(&c_e;&c_k;&c_ib;&c_p;&c_a;&c_zh;)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_mol;<klr> (&c_e;&c_k;&c_i;&c_p;&c_a;&c_zh;)</klr></trd>,
    <trd>&c_z;&c_a;&c_p;&c_r;&c_ja;&c_ch;&c_mol;<klr> (&c_e;&c_k;&c_i;&c_p;&c_a;&c_zh;)</klr></trd>
  </trdgrp>
</drv>

</art>
