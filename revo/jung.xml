<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc> <rad>jung</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="jung.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="jung.0i.RAJ">
    <dif>
      Alligi tirbeston a&ubreve; motoron al la tirota objekto:
      <ekz>
        <tld/>i &ccirc;evalon al veturilo;
      </ekz>
      <ekz>
        li <tld/>is sian &ccirc;aron<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la soldatoj sin <tld/>is al la kanono;
      </ekz>
      <ekz>
        ne <tld/>u la bovon malanta&ubreve; la plugilon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i lokomotivon al vagonaro;
      </ekz>
      <ekz>
        kiu sin en<tld/>is, devas tiri
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        por efektivigi tiun planon oni <tld/>is &ccirc;iujn plej eminentajn
        fortojn de la lando;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la homa spirito <tld/>is <klr>(obeigis)</klr> la naturajn
        fortojn.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_p;&c_r;&c_a;&c_g;&c_a;&c_c;&c_mol;</trd>
  <trd lng="br">sternia&ntilde;<klr tip="ind"> (loened-labour)</klr></trd>
  <trdgrp lng="de">
    <trd>einspannen</trd>,
    <trd>anschirren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>harness</trd>,
    <trd>yoke</trd>
  </trdgrp>
  <trd lng="fr">atteler</trd>
  <trdgrp lng="hu">
    <trd>befog <klr tip="ind">(kocsiba)</klr></trd>,
    <trd>felszersz&aacute;moz <klr tip="ind">(igavon&oacute;t)</klr></trd>,
    <trd>felkant&aacute;roz</trd>
  </trdgrp>
  <trd lng="nl">inspannen<klr tip="ind"> (v.dier)</klr></trd>
  <trdgrp lng="pt">
    <trd>atrelar</trd>,
    <trd>jungir</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_p;&c_r;&c_ja;&c_g;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_r;&c_ja;&c_ch;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="jung.0ilaro">
  <kap><tld/>ilaro</kap>
  <snc mrk="jung.0ilaro.RAJ">
    <dif>
      Aro de la rimenoj, &scirc;nuroj ktp uzataj por <tld/>i.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_p;&c_r;&c_a;&c_zh;</trd>
  <trdgrp lng="de">
    <trd>Zaumzeug</trd>,
    <trd>Pferdegeschirr</trd>,
    <trd>Geschirre</trd>
  </trdgrp>
  <trd lng="br">stern<klr tip="ind"> (loened-labour)</klr></trd>
  <trdgrp lng="en">
    <trd>harness</trd>,
    <trd>yoke</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>l&oacute;szersz&aacute;m</trd>,
    <trd>h&aacute;m</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>tuig</trd>,
    <trd>span</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_p;&c_r;&c_ja;&c_zh;&c_mol;</trd>
</drv>

<drv mrk="jung.0itaro">
  <kap><tld/>itaro</kap>
  <snc mrk="jung.0itaro.RAJ">
    <dif>
      <tld lit="J"/>itaj bestoj.
    </dif>
  </snc>
  <trd lng="br">tenn<klr tip="ind"> (b.)</klr></trd>
  <trd lng="en">team</trd>
  <trd lng="fr">attelage<klr tip="ind"> (animaux attel&eacute;s)</klr></trd>
  <trd lng="ru">&c_u;&c_p;&c_r;&c_ja;&c_zh;&c_k;&c_a;</trd>
</drv>

<drv mrk="jung.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="jung.mal0i.RAJ">
    <dif>
      Liberigi tirbeston el <tld/>ado:
      <ekz>
        mal<tld/>u tuj la &ccirc;evalojn kaj suprenigu ilin per &scirc;nuro
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>,
        <lok>&ccirc;apitro 21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>unharness</trd>,
    <trd>unyoke</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;teler</trd>
</drv>
</art>
