<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>viand</rad>/o
    </kap>

    <drv mrk="viand.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc>
        <dif>
          Muskola parto de animala <ref cel="karn.0o">karno</ref>,
          do escepte de hato kaj graso, kaj escepte de la karno de
          fi&scirc;oj, moluskoj kaj aliaj senvertebruloj, uzata por nutrado:
          <ekz>
            la &scirc;afid<tld/>o estas tre delikata;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            &gcirc;i estas <ind>nek <tld/>o, nek fi&scirc;o</ind>
            <klr>(= dubsenca)</klr>.
            (<refgrp tip="vid">
              <ref cel="abstin.0i">abstini</ref>,
              <ref cel="vegeta.0o">vegetarano</ref>;
            </refgrp>)
            <fnt><bib>PrV</bib></fnt>;
            <trd lng="hu">se hideg, se meleg</trd>
            <trd lng="ru">&c_n;&c_i; &c_r;&c_y;&c_b;&c_a;, &c_n;&c_i; &c_m;&c_ja;&c_s;&c_o;</trd>
          </ekz><ekz>
            <klr>(pilafo)</klr> estas preparata el kokina <tld/>o kaj
            rizo
            <fnt><bib>Fab1</bib>, <lok>Ligo de amikeco</lok></fnt>;
          </ekz><ekz>
            la tran&ccirc;ilo estis tiel malakra, ke mi ne povis tran&ccirc;i
            per &gcirc;i la <tld/>on
            <fnt><bib>F</bib>, <lok>&FE;34.</lok></fnt>;
          </ekz><ekz>
            <ind>li havis <tld/>on</ind>, mi havis nur oston
            &dash; li havis la &gcirc;uon, mi pagis la koston
            <fnt><bib>PrV</bib></fnt>.
            <trd lng="ru">&c_v; &c_ch;&c_u;&c_zh;&c_o;&c_m; &c_p;&c_i;&c_r;&c_u;
&c_p;&c_o;&c_h;&c_m;&c_je;&c_l;&c_mol;&c_je;</trd>
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_m;&c_ja;&c_s;&c_a;</trd>
      <trd lng="bg">&c_m;&c_je;&c_s;&c_o;</trd>
      <trd lng="br">kig</trd>
      <trd lng="ca">carn</trd>
      <trd lng="cs">maso</trd>
      <trd lng="de">Fleisch</trd>
      <trd lng="en">meat</trd>
      <trd lng="es">carne</trd>
      <trdgrp lng="fr">
        <trd>viande</trd>
      </trdgrp>
      <trd lng="hu">h&uacute;s</trd>
      <trd lng="nl">vlees<klr tip="ind"> (om te eten)</klr></trd>
      <trd lng="pt">carne</trd>
      <trd lng="ru">&c_m;&c_ja;&c_s;&c_o;</trd>
      <trd lng="sk">m&auml;so</trd>
      <trd lng="sv">k&ouml;tt</trd>
    </drv>

    <drv mrk="viand.0ejo">
      <kap><tld/>ejo</kap>
      <snc>
        <dif>
          <tld lit="V"/>ovendejo.
        </dif>
      </snc>
      <trd lng="ru">&c_m;&c_ja;&c_s;&c_n;&c_a;&c_ja; &c_l;&c_a;&c_v;&c_k;&c_a;</trd>
    </drv>

    <drv mrk="viand.0isto">
      <kap><tld/>isto</kap>
      <snc>
        <dif>
          <tld lit="V"/>ovendisto.
        </dif>
      </snc>
      <trd lng="ru">&c_m;&c_ja;&c_s;&c_n;&c_i;&c_k;</trd>
    </drv>

  </art>
