<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>viand</rad>/o
</kap>
<drv mrk="viand.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Karno, uzata por nutrado:
      <ekz>
        la &scirc;afid<tld/>o estas tre delikata
        <klr>(f)</klr>;
      </ekz>
      <ekz>&gcirc;i estas nek <tld/>o, nek fi&scirc;o
        <klr>(= dubsenca)</klr>.
        (
        <refgrp tip="vid">
          <ref cel="abstin.0i">abstini</ref>,
          <ref cel="vegeta.0o">vegetarano</ref>.
        </refgrp>
        )
<fnt><bib>PrV</bib></fnt></ekz>
    </dif>
  </snc>
  <trd lng="cs">maso</trd>
  <trd lng="de">Fleisch</trd>
  <trd lng="en">meat</trd>
  <trdgrp lng="fr">
    <trd>viande</trd>
  </trdgrp>
  <trd lng="nl">vlees<klr tip="ind"> (om te eten)</klr></trd>
  <trd lng="sv">k&ouml;tt</trd>
</drv>
</art>
