<drv mrk="viand.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="viand.0ejo.EKON">
    <dif>
      <tld lit="V"/>ovendejo:
      <ekz>
        virino estas &ccirc;e <tld/>ejo; &scirc;i mendas iom da bova&jcirc;o kaj iom da &scirc;afa&jcirc;o
        <fnt>
          <aut>G. Arosev</aut>
          <vrk>Kiom estas sep plus sep?</vrk>
          <lok>La Ondo de Esperanto, 2001:3 (77)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
      <trd lng="be">&c_m;&c_ja;&c_s;&c_n;&c_a;&c_ja; &c_k;&c_r;&c_a;&c_m;&c_a;</trd>
  <trd lng="ca">carnisseria</trd>
  <trdgrp lng="cs">
    <trd>prodejna masa</trd>,
    <trd>&rcaron;eznictv&iacute;</trd>
  </trdgrp>
  <trd lng="fr">boucherie<klr> (boutique)</klr></trd>
  <trd lng="hu">h&uacute;sbolt</trd>
  <trdgrp lng="pl">
        <trd>sklep <ind>mi&eogonek;sny</ind></trd>,
        <trd><klr tip="amb">sklep </klr><ind>mi&eogonek;sny</ind><klr>
            (pot.)</klr></trd>,
        <trd>masarnia</trd>
      </trdgrp>
  <trd lng="ru">&c_m;&c_ja;&c_s;&c_n;&c_a;&c_ja;
        &c_l;&c_a;&c_v;&c_k;&c_a;</trd>
  <trd lng="sk">m&auml;siarstvo</trd>
  <trd lng="uk">&c_m;&#8217;&c_ja;&c_s;&c_n;&c_a; &c_k;&c_r;&c_a;&c_m;&c_n;&c_i;&c_c;&c_ja;</trd>
</drv>

<drv mrk="viand.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="viand.0isto.EKON">
    <dif>
      <tld lit="V"/>ovendisto:
      <ekz>
        Araboj vendas fruktojn apud Kurdaj <tld/>istoj kaj Ka&ubreve;kaziaj panistoj
        <fnt>
          <aut>M. Court, C. Den Hond, trad. T. Tailhades</aut>
          <vrk><url ref="https://eo.mondediplo.com/article.php3?id_article=2528"
              >Utopio meze de la Siria kaoso</url></vrk>
          <lok>Le Monde diplomatique, 2017-09-01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="bucx.0isto">bu&ccirc;isto</ref>
  </refgrp>
  <trd lng="be">&c_m;&c_ja;&c_s;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trd lng="ca">carnisser</trd>
  <trdgrp lng="cs">
    <trd>prodava&ccaron; masa</trd>,
    <trd>&rcaron;ezn&iacute;k</trd>
  </trdgrp>
  <trd lng="fr">boucher<klr> (d&eacute;taillant)</klr></trd>
  <trd lng="hu">hentes</trd>
  <trdgrp lng="pl">
        <trd>masarz</trd>,
        <trd>sprzedawca w<klr> sklepie </klr>mi&eogonek;snym</trd>
      </trdgrp>
  <trd lng="ru">&c_m;&c_ja;&c_s;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">m&auml;siar</trd>
  <trd lng="uk">&c_m;&#8217;&c_ja;&c_s;&c_n;&c_i;&c_k;</trd>
</drv>

<drv mrk="viand.hak0ajxo">
  <kap>
    hak<tld/>a&jcirc;o,
    <var><kap>hak<tld/>o</kap></var>
  </kap>
  <snc mrk="viand.hak0ajxo.KUI">
    <dif>
      Frita&jcirc;o el hakita viando:
      <ekz>
        kiu estas la plej populara man&gcirc;a&jcirc;o en Japanio? &ccirc;u ekzemple 
        su&scirc;io a&ubreve; tempurao? ne ... plej popularas hak<tld/>a&jcirc;o
        <fnt>
          <aut>Igarasi Takeo</aut>
            <vrk><url ref="&Monato;/2000/007008.php">Internacia man&gcirc;a&jcirc;o, internacia
            lingvo</url></vrk>
            <bib>Monato</bib>
            <lok>2000/07, p. 9</lok>
          </fnt>;
      </ekz>
      <ekz>
        ni &ccirc;iam man&gcirc;is <klr>[&#8230;]</klr> merkrede, hak<tld/>on kun terpomoj
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2012/010361.php">Lisuarte</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Hamburgero <klr>[&#8230;]</klr>
        fari&gcirc;as el la sama speco de hak<tld/>o <klr>[kiel]</klr> aliaj hak<tld/>a&jcirc;oj, ekzemple
        <tld/>obuloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=33816">Hamburgero</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
      <trd>&c_s;&c_m;&c_a;&c_zh;&c_a;&c_n;&c_ib;&c_n;&c_a;</trd>,
      <trd>&c_s;&c_m;&c_a;&c_zh;&c_o;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
   <trdgrp lng="de">
      <trd>Hackfleisch</trd>,
      <trd>Bulette</trd>,
      <trd>Frikadelle</trd>,
      <trd>Fleischpflanzerl</trd>,
      <trd>Fleischk&uuml;chle</trd>,
      <trd>Beefsteak</trd>
  </trdgrp>
  <trd lng="fr">viande h&acirc;ch&eacute;e</trd>
</drv>

<!-- tio eble iru al mangx !!! -->
<drv mrk="viand.0omangxanto">
      <kap><tld/>oman&gcirc;anto</kap>
      <snc>
        <dif>
          Persono uzanta <tld/>on en sia nutrado:
          <ekz>
            la <ref tip="ant" cel="vegeta2.0ano">vegetarano</ref>
            &gcirc;uas pli bonan sanon, ne estas tiel forte
            elmetita al malsanoj a&ubreve; elportas tiujn &ccirc;i multe pli
            bone, ol la <tld/>oman&gcirc;anto
	    <fnt>
	      <vrk>Kio estas vegetarismo?</vrk>. <bib>FK</bib>
	    </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_m;&c_ja;&c_s;&c_a;&c_je;&c_d;</trd>
      <trd lng="ca">carn&iacute;vor<klr> (en oposici&oacute; a vegetari&agrave;)</klr></trd>
      <trd lng="hu">h&uacute;sev&odblac;</trd>
      <trd lng="ru">&c_m;&c_ja;&c_s;&c_o;&c_je;&c_d;</trd>
</drv>
