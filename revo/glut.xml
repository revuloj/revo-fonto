<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: glut.xml,v 1.29 2021/05/28 14:15:20 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>glut</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="glut.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="glut.0i.ANA">
    <dif>
      Malsuprenigi ion tra la gor&gcirc;o en la ezofagon kaj stomakon:
      <ekz>
        <tld/>i viandon, bieron, pilolon<fnt>B</fnt>;
      </ekz>
      <ekz>
        senma&ccirc;e <tld/>i;
      </ekz>
      <ekz>
        fi&scirc;o pli granda malgrandan en<tld/>as<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="ca">empassar-se</trd>
    <trdgrp lng="pt">
      <trd>engolir</trd>,
      <trd>tragar</trd>,
      <trd>deglutir</trd>
    </trdgrp>
  </snc>
  <snc mrk="glut.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malaperigi profunde en si:
      <ekz>
        la maro (en)<tld/>is la &scirc;ipon;
      </ekz>
      <ekz>
        la ocean' el bord' elirinta sova&gcirc;e <tld/>as l'
        ebena&jcirc;on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        densaj nuboj <tld/>is la sunon;
      </ekz>
      <ekz>
        <tld/>i per la okulo <klr>(avide rigardi, deziri)</klr>;
      </ekz>
      <ekz>
        tiu entrepreno <tld/>is grandajn kapitalojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <klr>[la tradukado]</klr> englutas ja neproduktive
        grandegan multon da laboroj kaj mono
        <fnt><bib>EE</bib>, <lok>&ccirc;apitro 2</lok></fnt>;
      </ekz>
      <ekz>
        li balda&ubreve; (en)<tld/>is multajn sciojn
        <klr>(rapide kaj maldetale lerni)</klr>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>engolir</trd>,
      <trd>englotir</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>engolir<klr tip="amb"> (fig.)</klr></trd>,
      <trd>tragar<klr tip="amb"> (fig.)</klr></trd>
    </trdgrp>
  </snc>
  <adm>
    Indus precizigi, kiam ~i kaj en~i ne sinonimas.
    <aut>MB</aut>
  </adm>
  <trd lng="bo">&#xF58;&#xF72;&#xF51;&#xF0B;&#xF41;&#xFB1;&#xF74;&#xF62;&#xF0B;</trd>
  <trdgrp lng="de">
    <trd>schlucken</trd>,
    <trd>verschlucken</trd>
  </trdgrp>
  <trd lng="es">tragar</trd>
  <trd lng="en">swallow</trd>
  <trdgrp lng="fr">
    <trd>avaler<klr tip="ind"> (ingurgiter)</klr></trd>,
    <trd>engloutir</trd>,
    <trd>ingurgiter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nyel</trd>,
    <trd>lenyel</trd>
  </trdgrp>
  <trdgrp lng="it">
      <trd>ingoiare</trd>,
      <trd>inghiottire</trd>,
      <trd>degluttire</trd>
    </trdgrp>
  <trdgrp lng="nl">
    <trd>slikken</trd>,
    <trd>inslikken</trd>
  </trdgrp>
  <trd lng="pl">&lstroke;yka&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_l;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_g;&c_l;&c_o;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">sv&auml;lja</trd>
  <trd lng="tp">moku</trd>
  <trd lng="tr">yutmak</trd>
  <trdgrp lng="sk">
    <trd>hlta&#x165;</trd>,
    <trd>prehltn&#xFA;&#x165;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>lokat</trd>,
    <trd>loknout</trd>,
    <trd>pohlcovat</trd>,
    <trd>polykat</trd>,
    <trd>spolknout</trd>
  </trdgrp>
</drv>

<drv mrk="glut.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="glut.0.SON">
    <dif>
      <ref tip="lst" cel="son.0o" lst="voko:sonoj">Sono</ref>
      de <tld/>ado:
      <ekz>
        li trinkadis labore.  <tld lit="G"/>...<tld/>...<tld/>...
        &Ccirc;u li eltenos?
        <fnt><bib>KielAkvo</bib>, <lok>2:1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>blub-blub</trd>,
    <trd>gloc-gloc</trd>
  </trdgrp>
  <trd lng="fr">glou !</trd>
  <trd lng="it">glu</trd>
</drv>



<drv mrk="glut.0o">
  <kap><tld/>o</kap>
  <snc mrk="glut.0o.ANA">
    <dif>
      Ago <tld/>i:
      <ekz>
        <ind>fari malbonan <tld/>on</ind><fnt>Z</fnt>;
        <trdgrp lng="ca">
          <trd>engargussar-se</trd>,
          <trd>ennuegar-se</trd>
        </trdgrp>
        <trd lng="fr">avaler de travers</trd>
        <trd lng="it">andare di traverso</trd>
      </ekz>
      <ekz>
        tusi pro mal&gcirc;usta en<tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>engolida</trd>,
      <trd>empassada</trd>,
      <trd>degluci&oacute;</trd>
    </trdgrp>
    <trd lng="hu">nyel&eacute;s</trd>
    <trd lng="it">deglutizione</trd>
    <trd lng="pt">engoli&ccedil;&atilde;o</trd>
  </snc>
  <snc mrk="glut.0o.MEZ">
    <dif>
      Kvanto de tio, kion oni <tld/>as per unu fojo; <tld/>kvanto:
      <ekz>
        li &ccirc;ion forman&gcirc;is per tri <tld/>oj.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>glop</trd>,
      <trd>mos, bocada</trd>
    </trdgrp>
    <trd lng="es">bocado</trd>
    <trd lng="fr">bouch&eacute;e</trd>
    <trd lng="hu">falat</trd>
    <trdgrp lng="it">
      <trd>sorsata</trd>,
      <trd>sorso</trd>,
      <trd>boccone<klr> (quantit&agrave;)</klr></trd>,
      <trd>boccata<klr> (quantit&agrave;)</klr></trd>
    </trdgrp>
    <trd lng="pl">&lstroke;yk</trd>
    <trdgrp lng="pt">
      <trd>gole</trd>,
      <trd>trago</trd>,
      <trd>sorvo</trd>
    </trdgrp>
    <trd lng="tr">lokma</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>dou&#x161;ek</trd>,
    <trd>hlt</trd>,
    <trd>lok</trd>,
    <trd>polknut&#xED;</trd>
  </trdgrp>
  <trd lng="de">Schluck</trd>
  <trd lng="nl">slok</trd>
  <trd lng="ru">&c_g;&c_l;&c_o;&c_t;&c_o;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>d&#xFA;&#x161;ok</trd>,
    <trd>hlt</trd>
  </trdgrp>
</drv>

<drv mrk="glut.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="glut.0ema.KOMUNE">
    <dif>
      Avide man&gcirc;anta, man&gcirc;egema.
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>golafre</trd>,
    <trd>vora&ccedil;</trd>,
    <trd>fartaner</trd>
  </trdgrp>
  <trd lng="cs">hltav&#xFD;</trd>
  <trd lng="es">trag&oacute;n</trd>
  <trd lng="fr">glouton<klr> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>torkos</trd>,
    <trd>fal&aacute;nk</trd>
  </trdgrp>
  <trd lng="it">ingordo</trd>
  <trd lng="nl">gulzig</trd>
  <trd lng="pl">&zdot;ar&lstroke;oczny</trd>
  <trd lng="pt">glut&atilde;o<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_zh;&c_o;&c_r;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
  <trd lng="sk">hltav&#xFD;</trd>
  <trd lng="tr">obur</trd>
</drv>

<drv mrk="glut.0ejo">
  <kap><tld/>ejo<fnt>Z</fnt></kap>
  <snc mrk="glut.0ejo.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Gor&gcirc;o.
    </dif>
    <trdgrp lng="ca">
      <trd>gola</trd>,
      <trd>gargamella</trd>
    </trdgrp>
    <trd lng="fr">gosier</trd>
    <trd lng="hu">torok</trd>
    <trd lng="it">gola</trd>
    <trd lng="nl">strot</trd>
    <trd lng="pl">paszcza</trd>
    <trd lng="pt">goela</trd>
    <trdgrp lng="ru">
      <trd>&c_g;&c_o;&c_r;&c_l;&c_o;</trd>,
      <trd>&c_g;&c_l;&c_o;&c_t;&c_k;&c_a;</trd>,
      <trd>&c_z;&c_je;&c_v;</trd>
    </trdgrp>
    <trd lng="sv">strupe</trd>
  </snc>
  <snc mrk="glut.0ejo.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Abismo.
    </dif>
    <refgrp tip="vid">
      <ref cel="fauxk.0o">fa&ubreve;ko</ref>.
    </refgrp>
    <trdgrp lng="ca">
      <trd>abisme</trd>,
      <trd>b&agrave;ratre</trd>
    </trdgrp>
    <trd lng="fr">gouffre<klr tip="ind"> (abime)</klr></trd>
    <trd lng="hu">szakad&eacute;k</trd>
    <trd lng="it">voragine<klr> (fig.)</klr></trd>
    <trd lng="nl">afgrond<klr tip="ind"> (fig.)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_p;&c_a;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_s;&c_t;&c_mol;<klr tip="ind"> (&c_p;&c_je;&c_r;&c_je;&c_n;.)</klr></trd>
    </trdgrp>
    <trd lng="sv">avgrund</trd>
  </snc>
  <trd lng="cs">propast</trd>
  <trd lng="de">Rachen</trd>
  <trdgrp lng="sk">
    <trd>hltan</trd>,
    <trd>priepas&#x165; <klr>(prenesene)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="glut.for0i">
  <kap>for<tld/>i</kap>
  <snc mrk="glut.for0i.KOMUNE">
    <dif>
      Malaperigi <tld/>ante:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        for<tld/>i <klr>(malklare, duone elparoli)</klr> la
        vortojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li silentis, for<tld/>ante siajn timojn.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>empassar-se</trd>,
    <trd>empassolar-se</trd>
  </trdgrp>
  <trd lng="cs">zhltnout</trd>
  <trd lng="de">verschlucken</trd>
  <trdgrp lng="fr">
    <trd>avaler<klr tip="ind"> (faire dispara&icirc;tre)</klr></trd>,
    <trd>ravaler<klr tip="ind"> (r&eacute;primer)</klr></trd>,
    <trd>r&eacute;primer<klr tip="ind"> (ravaler)</klr></trd>
  </trdgrp>
  <trd lng="hu">elnyel</trd>
  <trdgrp lng="it">
    <trd>trangugiare</trd>,
    <trd>ingollare</trd>
  </trdgrp>
  <trd lng="nl">inslikken<klr tip="ind"> (fig.)</klr></trd>
  <trd lng="pl">po&lstroke;yka&cacute;</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_g;&c_l;&c_o;&c_t;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>pohlti&#x165;</trd>,
    <trd>zhltn&#xFA;&#x165;</trd>
  </trdgrp>
</drv>
</art>
