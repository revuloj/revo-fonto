      Avide man&gcirc;anta, man&gcirc;egema.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_zh;&c_e;&c_r;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_n;&c_a;&c_je;&c_d;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>golafre</trd>,
    <trd>vora&ccedil;</trd>,
    <trd>fartaner</trd>
  </trdgrp>
  <trd lng="cs">hltav&yacute;</trd>
  <trd lng="es">trag&oacute;n</trd>
  <trd lng="fr">glouton<klr> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>torkos</trd>,
    <trd>fal&aacute;nk</trd>
  </trdgrp>
  <trd lng="it">ingordo</trd>
  <trd lng="nl">gulzig</trd>
  <trd lng="pl">&zdot;ar&lstroke;oczny</trd>
  <trd lng="pt">glut&atilde;o<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_zh;&c_o;&c_r;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
  <trd lng="sk">hltav&yacute;</trd>
  <trd lng="tr">obur</trd>
  <trd lng="uk">&c_z;&c_a;&c_h;&c_l;&c_a;&c_n;&c_n;&c_i;&c_j;</trd>
</drv>

