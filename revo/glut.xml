  <trd lng="cs">propast</trd>
  <trd lng="de">Rachen</trd>
  <trdgrp lng="sk">
    <trd>hltan</trd>,
    <trd>priepas&tcaron; <klr>(prenesene)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_ib;&c_v;</trd>,
    <trd>&c_p;&c_a;&c_shch;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="glut.en0i">
  <kap>
    en<tld/>i
  </kap>
  <snc>
    <dif>
      Tute <ref tip="super" cel="en.0igi.KOMUNE">enigi</ref> en sin:
      <ekz>
        la juna virino en<tld/>is la enhavon de l&#8217; glaso per unu trinko
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la grandaj buldogoj <klr>[&#8230;]</klr> aspektis kvaza&ubreve; &ccirc;iu el ili povus en<tld/>i
        homon
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la tero <klr>[&#8230;]</klr> en<tld/>os ilin, kaj &ccirc;ion, kio apartenas al ili
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 16:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        multaj en mia a&gcirc;o <klr>[&#8230;]</klr> en<tld/>is la tutan sa&gcirc;econ de la mondo
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        per sola trarigardo &scirc;i en<tld/>is la tutan artikolon
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i tiu bildo en<tld/>is <klr>(<ref cel="posed.ek0i">ekposedis</ref>, 
        <ref cel="fascin.0i">fascinis</ref>, <ref cel="okup.0i.laborigi">okupis<sncref/></ref>)</klr>
        &scirc;iajn pensojn, tamen videble &scirc;in ne teruris
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        granda fi&scirc;o en<tld/>is la hoketon, kaj la malforta krea&jcirc;o ne havis sufi&ccirc;e de
        fortoj por eltiri &gcirc;in
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino kaj Rozulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ia unu popolo dispremos kaj en<tld/>os &ccirc;iujn aliajn popolojn <klr>[per]</klr>
        grandega superforto
        <fnt>
          <bib>EE</bib>
          <lok>II</lok>
        </fnt>;
      </ekz>
      <ekz>
        fi&scirc;o pli granda malgrandan en<tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        se vi krudulon salutos, li vin tutan en<tld/>os
        <klr>(<ref cel="posed.ek0i.okupi">ekposedos</ref>)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="glut.for0i">forgluti</ref>,      
      <ref cel="vor.0i">vori</ref>
    </refgrp>
  </snc>
  <snc mrk="glut.en0i.FIG">
    <dif>
      <ref cel="konsum.for0i">Forkonsumi</ref>, <ref cel="uz.for0i">foruzi</ref>:
      <ekz>
        tio &ccirc;i en<tld/>as ja neproduktive grandegan multon da laboroj kaj mono
        <fnt>
          <bib>EE</bib>
          <lok>II</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i tiuj virinoj <klr>[&#8230;]</klr> en<tld/>as la ri&ccirc;a&jcirc;ojn <klr>[&#8230;]</klr> miajn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
    </dif>
  </snc>
  <trd lng="de">verschlingen</trd>
  <trd lng="en">devour</trd>
  <trd lng="fr">d&eacute;vorer</trd>
  <trd lng="ru">&c_p;&c_o;&c_zh;&c_i;&c_r;&c_a;&c_t;&c_mol;</trd>
</drv>





<drv mrk="glut.for0i">
  <kap>for<tld/>i</kap>
  <snc mrk="glut.for0i.KOMUNE">
    <dif>
      Malaperigi <tld/>ante:
      <ekz>
        leono blekeganta &ccirc;irka&ubreve;iras, ser&ccirc;ante, kiun li povos for<tld/>i
        <fnt>
          <bib>NT</bib>
          <lok>I. Petro 5:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiun trezoron for<tld/>is la kameno
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        for<tld/>i <klr>(malklare, duone elparoli)</klr> la
        vortojn
        <fnt><bib>PV</bib></fnt>;
      </ekz>
      <ekz>
        li silentis, for<tld/>ante siajn timojn
        <fnt><bib>PV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la fortoj de la tutmondi&gcirc;o iam for<tld/>as &ccirc;iujn diferencojn
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470; 10</lok>
        </fnt>;
      </ekz>
      <ekz>
        nigraj truoj kapablas ne nur for<tld/>i, entiri internen, la &ccirc;irka&ubreve;an materion,
        sed male &gcirc;in e&ccirc; elradii
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="https://www.monato.be/2013/010982.php"
              >La&ubreve;do de moderna superhomo</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_g;&c_l;&c_y;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_g;&c_l;&c_y;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>empassar-se</trd>,
    <trd>empassolar-se</trd>
  </trdgrp>
  <trd lng="cs">zhltnout</trd>
  <trd lng="de">verschlucken</trd>
  <trdgrp lng="fr">
    <trd>avaler<klr tip="ind"> (faire dispara&icirc;tre)</klr></trd>,
    <trd>ravaler<klr tip="ind"> (r&eacute;primer)</klr></trd>,
    <trd>r&eacute;primer<klr tip="ind"> (ravaler)</klr></trd>
  </trdgrp>
  <trd lng="hu">elnyel</trd>
  <trdgrp lng="it">
    <trd>trangugiare</trd>,
    <trd>ingollare</trd>
  </trdgrp>
  <trd lng="nl">inslikken<klr tip="ind"> (fig.)</klr></trd>
  <trd lng="pl">po&lstroke;yka&cacute;</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_g;&c_l;&c_o;&c_t;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>pohlti&tcaron;</trd>,
    <trd>zhltn&uacute;&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_p;&c_r;&c_o;&c_k;&c_o;&c_v;&c_t;&c_n;&c_u;&c_t;&c_i;</trd>
</drv>
</art>
