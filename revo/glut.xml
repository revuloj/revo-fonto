    </trdgrp>
    <trd lng="sv">avgrund</trd>
  </snc>
  <trd lng="cs">propast</trd>
  <trd lng="de">Rachen</trd>
  <trdgrp lng="ja">
    <trd>&#x306E;&#x3069;</trd>,
    <trd>&#x6DF1;&#x3044;&#x88C2;&#x3051;&#x76EE;  <pr>&#x3075;&#x304B;&#x3044;&#x3055;&#x3051;&#x3081;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hltan</trd>,
    <trd>priepas&tcaron; <klr>(prenesene)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_ib;&c_v;</trd>,
    <trd>&c_p;&c_a;&c_shch;&c_a;</trd>
