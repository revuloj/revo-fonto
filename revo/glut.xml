    <dif>
      Gor&gcirc;o.
    </dif>
    <trd lng="be">&c_g;&c_l;&c_o;&c_t;&c_k;&c_a;</trd>
    <trdgrp lng="ca">
      <trd>gola</trd>,
      <trd>gargamella</trd>
    </trdgrp>
    <trd lng="fr">gosier</trd>
    <trd lng="hu">torok</trd>
    <trd lng="it">gola</trd>
    <trd lng="nl">strot</trd>
    <trd lng="pl">paszcza</trd>
    <trd lng="pt">goela</trd>
    <trdgrp lng="ru">
      <trd>&c_g;&c_o;&c_r;&c_l;&c_o;</trd>,
      <trd>&c_g;&c_l;&c_o;&c_t;&c_k;&c_a;</trd>,
      <trd>&c_z;&c_je;&c_v;</trd>
