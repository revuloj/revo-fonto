<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stronci.xml,v 1.23 2024/03/25 07:28:37 revo Exp $">
<kap>
  <rad>stronci</rad>/o
</kap>

<drv mrk="stronci.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <mlg kod="IUPAC">Sr</mlg>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="38">elemento</ref> kun
      atomnumero 38, atompezo 87,62;
      <ref tip="super" cel="metal.teralkala0o">teralkala metalo</ref>:
      <ekz>
        150&#x202F;000 kvadrataj kilometroj da teritorio en Ukrainio, Belorusio kaj Rusio
        estis preska&ubreve; tuj deklaritaj poluitaj (kun niveloj de radiado, kiuj atingis
        38&#x202F;000 bekerelojn por kvadrata metro), &ccirc;efe fare de izotopoj de cezio, <tld/>o kaj jodo
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2015/011522.php">La akcidento de &Ccirc;ernobilo 30 jarojn poste</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_r;&c_o;&c_n;&c_c;&c_y;&c_j;</trd>
  <trd lng="bg">&c_s;&c_t;&c_r;&c_o;&c_n;&c_c;&c_i;&c_j;</trd>
  <trd lng="br">strontiom</trd>
  <trd lng="ca">estronci</trd>
  <trdgrp lng="cs">
    <trd>Sr</trd>,
    <trd>stroncium</trd>
  </trdgrp>
  <trd lng="de">Strontium</trd>
  <trd lng="en">strontium</trd>
  <trd lng="es">estroncio</trd>
  <trd lng="fa">&a_A;&a_s;&a_t;&a_r;&a_w;&a_n;&a_s;&f_y;&a_m;</trd>
  <trd lng="fr">strontium</trd>
  <trd lng="hu">stroncium</trd>
  <trd lng="id">stronsium</trd>
  <trd lng="it">stronzio</trd>
  <trd lng="ja">&#x30B9;&#x30C8;&#x30ED;&#x30F3;&#x30C1;&#x30A6;&#x30E0;</trd>
  <trd lng="la">Strontium</trd>
  <trd lng="nl">strontium</trd>
  <trd lng="pl">stront</trd>
  <trd lng="pt">estr&ocirc;ncio</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_o;&c_n;&c_c;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>Sr</trd>,
    <trd>stroncium</trd>
  </trdgrp>
  <trd lng="sv">strontium</trd>
  <trd lng="uk">&#x441;&#x442;&#x440;&#x43E;&#x43D;&#x446;&#x456;&#x439;</trd>
  <trdgrp lng="zh">
    <trd>&#x9376; <pr>s&#x12B;</pr></trd>,
    <trd>&#x9536; <pr>s&#x12B;</pr></trd>,
    <trd>&#x93B4; <pr>x&#xED;</pr></trd>
  </trdgrp>
</drv>
</art>
