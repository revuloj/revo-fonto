<drv mrk="renkon.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="renkon.0i.kunigxi">
    <dif>
      Trovi iun irante kontra&ubreve;direkte al li:
      <ekz>
        hiera&ubreve; mi <tld/>is vian filon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la unua <tld/>ita bubo rigardas ilin de alte<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kruc.0igxi">kruci&gcirc;i</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>begegnen</trd>,
      <trd>treffen</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>cruzarse con</trd>
    </trdgrp>
    <trd lng="hu">tal&aacute;lkozik</trd>
    <trd lng="pt">encontrar(-se) com</trd>
  </snc>
  <snc mrk="renkon.0i.trafi">
    <dif>
      Kunpu&scirc;i&gcirc;i kun, ektu&scirc;egi,
      <ref tip="vid" cel="kolizi.0i">kolizii</ref>,
      <ref tip="vid" cel="traf.0i">trafi</ref>:
      <ekz>
        la &scirc;ipo <tld/>is &scirc;tonegon.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>treffen auf</trd>,
      <trd>sto&szlig;en auf</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>colisionar</trd>,
      <trd>chocar</trd>
    </trdgrp>
    <trd lng="fr">heurter</trd>
    <trd lng="hu">nekimegy</trd>
  </snc>
  <snc mrk="renkon.0i.trovi">
    <dif>
      Trovi, ricevi, sperti iun okaza&jcirc;on,
      <ref tip="vid" cel="traf.0i">trafi</ref>:
      <ekz>
        oni ne <tld/>as en tiu libro la necesan unuecon;
      </ekz>
      <ekz>
        la unua objekto, kiun <tld/>is lia rigardo, estis la
        gasto<fnt>B</fnt>;
      </ekz>
      <ekz>
        la pioniroj de novaj ideoj <tld/>as plej ofte mokon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i malfacila&jcirc;ojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i plenan fiaskon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i atenton, aprobon, simpation &ccirc;e iu;
      </ekz>
      <ekz>
        liaj klopodoj ne &ccirc;iam <tld/>is bonan komprenon.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>erfahren</trd>,
      <trd>finden</trd>,
      <trd>sto&szlig;en auf <klr>(fig.)</klr></trd>
    </trdgrp>
    <trd lng="es">hallar</trd>
    <trd lng="fr">trouver</trd>
    <trdgrp lng="hu">
      <trd>tal&aacute;l</trd>,
      <trd>kap</trd>,
      <trd>r&eacute;szes&uuml;l</trd>
    </trdgrp>
  </snc>
  <snc mrk="renkon.0i.rigardi">
    <dif>
      Opinii iamaniere pri iu a&ubreve; io, rigardi el ia vidpunkto:
      <ekz>
        la publiko <tld/>is tiun ideon kun malkonfido<fnt>Z</fnt>;
      </ekz>
      <ekz>
        multe ni jam devis suferi de tiu malvasta
        pse&ubreve;dopatriotismo, kun
        kiu diversaj personoj <tld/>is nian aferon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ili malamike <tld/>as nian aferon<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>finden <klr>(fig.)</klr></trd>,
      <trd>begegnen mit <klr>(fig.)</klr></trd>
    </trdgrp>
    <trd lng="hu">fogad</trd>    
  </snc>
  <trd lng="af">ontmoet</trd>
  <trd lng="am">&#x1218;&#x1308;&#x1293;&#x1298;&#x1275;</trd>
  <trd lng="ar">&#x62A;&#x644;&#x628;&#x64A;&#x629;</trd>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_s;&c_t;&c_r;&c_a;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_u;&c_s;&c_t;&c_r;&c_e;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_t;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bn">&#x9A6;&#x9C7;&#x996;&#x9BE;</trd>
  <trd lng="bo">&#xF60;&#xF55;&#xFB2;&#xF51; &#xF55;&#xFB2;&#xF51;&#xF0B;</trd>
  <trd lng="co">s&#xEC;mula</trd>
  <trd lng="cs">potkat</trd>
  <trd lng="da">m&#xF8;d</trd>
  <trd lng="en">encounter</trd>
  <trd lng="es">disentir</trd>
  <trd lng="eu">bete</trd>
  <trd lng="fr">rencontrer</trd>
  <trd lng="fy">moetsje</trd>
  <trd lng="gl">meet</trd>
  <trd lng="gu">&#xAAA;&#xAC2;&#xAB0;&#xAC0;</trd>
  <trd lng="ha">sadu da</trd>
  <trd lng="hi">&#x92E;&#x93F;&#x932;&#x94B;</trd>
  <trd lng="hr">upoznajte</trd>
  <trd lng="hy">&#x570;&#x561;&#x576;&#x564;&#x56B;&#x57A;&#x578;&#x582;&#x574;</trd>
  <trd lng="is">m&#xE6;ta</trd>
  <trd lng="ja">&#x5927;&#x4F1A;</trd>
  <trd lng="jw">ketemu</trd>
  <trd lng="ka">&#x10E8;&#x10D4;&#x10EE;&#x10D5;&#x10D3;&#x10D4;&#x10D1;&#x10D0;</trd>
  <trd lng="km">&#x1787;&#x17BD;&#x1794;</trd>
  <trd lng="ko">&#xB300;&#xD68C;</trd>
  <trd lng="lo">&#xE81;&#xEAD;&#xE87;&#xE9B;&#xEB0;&#xE8A;&#xEB8;&#xEA1;</trd>
  <trd lng="ml">&#xD38;&#xD02;&#xD17;&#xD2E;&#xD02;</trd>
  <trd lng="mr">&#x92C;&#x948;&#x920;&#x915;</trd>
  <trd lng="my">&#x1006;&#x102F;&#x1036;</trd>
  <trd lng="ne">&#x92C;&#x948;&#x920;&#x915;</trd>
  <trdgrp lng="nl">
    <trd>ontmoeten</trd>,
    <trd>tegenkomen</trd>
  </trdgrp>
  <trd lng="ny">kudzakhalire</trd>
  <trd lng="pa">&#xA2E;&#xA3F;&#xA32;&#xA26;&#xA47; &#xA39;&#xA28;</trd>
  <trd lng="ps">&#x644;&#x6CC;&#x62F;&#x646;&#x647; &#x6A9;&#x648;&#x64A;</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_s;&c_t;&c_r;&c_je;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_s;&c_t;&c_r;&c_je;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sd">&#x645;&#x644;&#x646; &#x67F;&#x627;</trd>
  <trd lng="si">&#xDAD;&#xDBB;&#xD9C;</trd>
  <trd lng="sk">stretn&uacute;&tcaron;</trd>
  <trd lng="sq">takohen</trd>
  <trd lng="ta">&#xB9A;&#xBA8;&#xBCD;&#xBA4;&#xBBF;&#xB95;&#xBCD;&#xB95;</trd>
