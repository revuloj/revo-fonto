<drv mrk="renkon.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc mrk="renkon.0igxo.kunveno">
    <dif>
      Inter<tld/>o,
      <ref tip="vid" cel="ven.kun0o.KOMUNE">kunveno</ref>:
      <ekz>
        &ccirc;e la <tld/>i&gcirc;o ili rekonadis sin reciproke
        <fnt>
          <bib>Fab2</bib>
          <lok>Najbaraj familioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fra&ubreve;lineto videble ne estis malkontenta de la <tld/>i&gcirc;o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        varmigi en ni per reciproka <tld/>i&gcirc;o kaj kunvivo
        la amon kaj entuziasmon por la ideo, kiun la
        Esperantismo en si enhavas
        <fnt>
          <bib>Paroloj</bib>,
          <vrk>Parolado anta&ubreve; la tria kongreso en Kembri&gcirc;o,
          1907</vrk>;
        </fnt>
      </ekz>
      <ekz>
        <klr>[mi]</klr> tuj penos aran&gcirc;i en ia maniero <tld/>i&gcirc;on inter li kaj mia filino
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        Laerto en dekdu <tld/>i&gcirc;oj <klr>(kp <ref cel="macx1.0o">ma&ccirc;o</ref>,
        <ref cel="rauxnd.0o">ra&ubreve;ndo</ref>)</klr>
        inter vi kaj li ne venkos pli ol tri fojojn
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la subita <tld/>i&gcirc;o kun lia respekteginda avo <klr>[&#8230;]</klr> vivece revokis al
        li memorojn
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu unua <tld/>i&gcirc;o inter la oratoro kaj la publiko estas tre grava por la plua
        sekvo de la parolado
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_u;&c_s;&c_t;&c_r;&c_e;&c_ch;&c_a;</trd>,
      <trd>&c_s;&c_p;&c_a;&c_t;&c_k;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trd lng="de">Treffen</trd>
    <trd lng="es">reuni&oacute;n</trd>
    <trdgrp lng="hu">
      <trd>tal&aacute;lkoz&aacute;s</trd>,
     <trd>&ouml;sszej&ouml;vetel</trd>
    </trdgrp>
    <trd lng="nl">bijeenkomst</trd>
    <trd lng="ru">&c_v;&c_s;&c_t;&c_r;&c_je;&c_ch;&c_a;</trd>
  </snc>
  <snc mrk="renkon.0igxo.kunokazo">
    <dif>
      Kunokazi&gcirc;o,
      <ref tip="vid" cel="koinci.0o">koincido</ref>:
      <ekz>
        stranga <ind><tld/>i&gcirc;o de la cirkonstancoj</ind>
        <fnt>
          <bib>LR</bib>
          <lok>Unua Parto &ndash; Leksikologio</lok>
        </fnt>.
        <trdgrp lng="be">
          <trd>&c_z;&c_mol;&c_b;&c_je;&c_g;
          &c_a;&c_k;&c_a;&c_l;&c_ib;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_w;</trd>,
          <trd>&c_z;&c_mol;&c_b;&c_je;&c_g;
          &c_v;&c_a;&c_r;&c_u;&c_n;&c_k;&c_a;&c_w;</trd>
        </trdgrp>
        <trd lng="de"><ind>Zusammentreffen</ind> von Umst&auml;nden</trd>
        <trd lng="es">c&uacute;mulo de circunstancias</trd>
        <trd lng="fr">concours de circonstances</trd>
        <trd lng="hu">k&ouml;r&uuml;lm&eacute;nyek
        egybees&eacute;se</trd>
        <trd lng="ru">&c_s;&c_t;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;
        &c_o;&c_b;&c_s;&c_t;&c_o;&c_ja;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_u;&c_p;&c_a;&c_d;&c_a;&c_k;</trd>
    <trd lng="de">Zusammentreffen</trd>
    <trd lng="es">coincidencia</trd>
    <trd lng="hu">egybees&eacute;s</trd>
    <trd lng="ru">&c_s;&c_o;&c_v;&c_p;&c_a;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <rim>
    &leftquot;<tld lit="R"/>o&rightquot; kaj&leftquot; <tld/>i&gcirc;o&rightquot; estas preska&ubreve; samsignifaj, sed pro ties transitiveco la
    unua pli akcentas, kun kiu oni kunvenas; dum la dua emfazas, kio okazas dum
    la kunesto.
    <aut>Wolfram Diestel</aut>
  </rim>
  <trd lng="bg">&c_s;&c_r;&c_je;&c_shch;&c_a;</trd>
  <trd lng="es">encuentro</trd>
  <trd lng="fr">rencontre</trd>
  <trdgrp lng="id">
    <trd><baz>temu</baz>pertemuan</trd>,
    <trd><baz>sua</baz>persuaan</trd>,
    <trd><baz>jumpa</baz>perjumpaan</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x51FA;&#x4F1A;&#12356;  <pr>&#12391;&#12354;&#12356;</pr></trd>,
    <trd>&#x906D;&#36935;  <pr>&#x305D;&#12358;&#12368;&#12358;</pr></trd>,
    <trd>&#x4F1A;&#21512;  <pr>&#x304B;&#12356;&#12372;&#12358;</pr></trd>,
    <trd>&#x96C6;&#12356;  <pr>&#12388;&#12393;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">samenloop</trd>
  <trdgrp lng="pl">
    <trd>spotkanie</trd>,
    <trd>zlot</trd>,
    <trd>zjazd</trd>
  </trdgrp>
  <trd lng="uk">&c_z;&c_u;&c_s;&c_t;&c_r;&c_ib;&c_ch;</trd>
</drv>

