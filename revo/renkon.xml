  <trd lng="ur">&#x645;&#x644;&#x648;</trd>
  <trd lng="vi">&#x111;&#xE1;p &#x1EE9;ng</trd>
  <trd lng="yi">&#x5D8;&#x5E8;&#x5E2;&#x5E4;&#x5DF;</trd>
  <trd lng="zu">hlangana</trd>
</drv>

<drv mrk="renkon.0eal">
  <kap><tld/>e al</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="renkon.0eal.KOMUNE">
    <dif>
      Irante en kontra&ubreve;a direkto kaj proksimi&gcirc;ante al:
      <ekz>
        <tld/>e al li iras virino<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>naproti</trd>,
    <trd>vst&rcaron;&iacute;c</trd>
  </trdgrp>
  <trd lng="es">al encuentro de</trd>
  <trd lng="fr">&agrave; la rencontre de</trd>
  <trdgrp lng="sk">
    <trd>naproti</trd>,
    <trd>v &uacute;strety</trd>
  </trdgrp>
</drv>

<drv mrk="renkon.0igxikun">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="renkon.0igxikun.kunveni">
    <dif>
      Intervidi&gcirc;i kun,
      hazarde a&ubreve; nehazarde kuni&gcirc;i kun:
      <ekz>
        irante de tie, mi <tld/>i&gcirc;is kun Pjotr Ivanovi&ccirc;
        <fnt><bib>Revizoro</bib></fnt>;
      </ekz>
      <ekz>
        &scirc;ia rigardo <tld/>i&gcirc;is kun la rigardo de la libristo
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_u;&c_s;&c_t;&c_r;&c_e;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_s;&c_p;&c_a;&c_t;&c_k;&c_a;&c_c;&c_c;&c_a;</trd>
    </trdgrp>
    <trd lng="en">meet</trd>
    <trd lng="es">encontrarse</trd>
    <trdgrp lng="fr">
      <trd>se rencontrer</trd>,
      <trd>se r&eacute;unir</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>tal&aacute;lkozik</trd>,
      <trd>&ouml;sszej&ouml;n</trd>
    </trdgrp>
    <trd lng="nl">elkaar ontmoeten</trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_s;&c_t;&c_r;&c_je;&c_t;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_v;&c_s;&c_t;&c_r;&c_je;&c_ch;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc mrk="renkon.0igxikun.kunokazi">
    <dif>
      Kunokazi,
      <ref tip="vid" cel="koinci.0i">koincidi</ref>.
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_u;&c_p;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_u;&c_p;&c_a;&c_d;&c_a;&c_c;&c_c;&c_a;</trd>
    </trdgrp>
    <trd lng="es">coincidir</trd>
    <trd lng="fr">rencontrer</trd>
    <trd lng="hu">egybeesik</trd>    
    <trd lng="ru">&c_s;&c_o;&c_v;&c_p;&c_a;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="de">zusammentreffen</trd>
</drv>

<drv mrk="renkon.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc mrk="renkon.0igxo.kunveno">
    <dif>
      Inter<tld/>o,
      <ref tip="vid" cel="ven.kun0o.KOMUNE">kunveno</ref>:
      <ekz>
        varmigi en ni per reciproka <tld/>i&gcirc;o kaj kunvivo
        la amon kaj entuziasmon por la ideo, kiun la
        Esperantismo en si enhavas
        <fnt>
          <bib>Paroloj</bib>,
          <vrk>Parolado anta&ubreve; la tria kongreso en Kembri&gcirc;o,
          1907</vrk>.
        </fnt>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_u;&c_s;&c_t;&c_r;&c_e;&c_ch;&c_a;</trd>,
      <trd>&c_s;&c_p;&c_a;&c_t;&c_k;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trd lng="de">Treffen</trd>
    <trd lng="es">reuni&oacute;n</trd>
    <trdgrp lng="hu">
      <trd>tal&aacute;lkoz&aacute;s</trd>,
     <trd>&ouml;sszej&ouml;vetel</trd>
    </trdgrp>
    <trd lng="nl">bijeenkomst</trd>
    <trd lng="ru">&c_v;&c_s;&c_t;&c_r;&c_je;&c_ch;&c_a;</trd>
  </snc>
  <snc mrk="renkon.0igxo.kunokazo">
    <dif>
      Kunokazi&gcirc;o,
      <ref tip="vid" cel="koinci.0o">koincido</ref>:
      <ekz>
        stranga <ind><tld/>i&gcirc;o de la
        cirkonstancoj</ind><fnt>Z</fnt>.
        <trdgrp lng="be">
          <trd>&c_z;&c_mol;&c_b;&c_je;&c_g;
          &c_a;&c_k;&c_a;&c_l;&c_ib;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_w;</trd>,
          <trd>&c_z;&c_mol;&c_b;&c_je;&c_g;
          &c_v;&c_a;&c_r;&c_u;&c_n;&c_k;&c_a;&c_w;</trd>
        </trdgrp>
        <trd lng="es">c&uacute;mulo de circunstancias</trd>
        <trd lng="fr">concours de circonstances</trd>
        <trd lng="hu">k&ouml;r&uuml;lm&eacute;nyek
        egybees&eacute;se</trd>        
        <trd lng="ru">&c_s;&c_t;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;
        &c_o;&c_b;&c_s;&c_t;&c_o;&c_ja;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_u;&c_p;&c_a;&c_d;&c_a;&c_k;</trd>
    <trd lng="de">Zusammentreffen</trd>
    <trd lng="es">&c_s;oincidencia</trd>
    <trd lng="hu">egybees&eacute;s</trd>    
    <trd lng="ru">&c_s;&c_o;&c_v;&c_p;&c_a;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <trd lng="bg">&c_s;&c_r;&c_je;&c_shch;&c_a;</trd>
  <trd lng="es">encuentro</trd>
  <trd lng="fr">rencontre</trd>
  <trd lng="nl">samenloop</trd>
</drv>
