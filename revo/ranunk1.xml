<art mrk="$Id: ranunk1.xml,v 1.20 2024/04/13 20:15:51 revo Exp $">
<kap>
  <rad>ranunkolac</rad>/oj
</kap>
<drv mrk="ranunk1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Ranunculaceae</trd>):
      <ekz>
        anemono estas genro de <klr>[&#x2026;]</klr> <tld/>oj <klr>[&#x2026;]</klr>, kiel &ccirc;iuj
        <tld/>oj, ili havas kunmetitajn foliojn, kiuj havas tra&hcirc;eojn en pluraj cirkloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=290592">Anemono</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_z;&c_ja;&c_l;&c_mol;&c_c;&c_o;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#x2019;&c_ja;)</klr></trd>
  <trd lng="ca">ranuncul&agrave;cies</trd>
  <trd lng="de">Hahnenfu&szlig;gew&auml;chse</trd>
  <trd lng="es">ranuncul&aacute;ceas</trd>
  <trd lng="fr">renonculac&eacute;es</trd>
  <trd lng="hu">bogl&aacute;rkaf&eacute;l&eacute;k</trd>
  <trd lng="ja">&#x30AD;&#x30F3;&#x30DD;&#x30A6;&#x30B2;&#x79D1;  <pr>&#x30AD;&#x30F3;&#x30DD;&#x30A6;&#x30B2;&#x304B;</pr></trd>
  <trd lng="nl">ranonkelfamilie</trd>
  <trd lng="pl">jaskrowate</trd>
  <trd lng="pt">ranuncol&aacute;ceas</trd>
  <trd lng="ro">ranunculaceae</trd>
  <trd lng="ru">&c_l;&c_ju;&c_t;&c_i;&c_k;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sv">ranunkelv&auml;xter</trd>
  <trd lng="uk">&c_zh;&#x43E;&c_v;&c_t;&c_je;&c_c;&c_je;&c_v;&c_ib;</trd>
  <trd lng="zh">&#x6BDB;&#x831B;&#x79D1; <pr>m&#xE1;og&#xE8;nk&#x113;</pr></trd>
</drv>
