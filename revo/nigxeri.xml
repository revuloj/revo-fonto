<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ni&gcirc;eri</rad>/o
</kap>

<drv mrk="nigxeri.0o">
  <kap>
    <tld lit="N"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="nigxeri.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o.POL">&Scirc;tato</ref> en la centra 
      <ref tip="malprt" cel="afrik.A0o">Afriko</ref>
      kun &ccirc;efurbo Abu&gcirc;o.
      Teritorio de Federala Respubliko Ni&gcirc;erio.
      ISO-3166 kodo: NG.
      &Gcirc;i limas kun
      <ref tip="vid" cel="benin.0o.GEOG">Benino</ref>,
      <ref tip="vid" cel="cxad.0o.GEOG">&Ccirc;ado</ref>,
      <ref tip="vid" cel="kamerun.0o.GEOG">Kameruno</ref>,
      <ref tip="vid" cel="nigxer.0o.GEOG">Ni&gcirc;ero</ref>
      kaj <ref tip="vid" cel="atlant.0o.GEOG">Atlantiko</ref> (Gvinea 
      Golfo).
    </dif>
    <trd lng="bg">&c_N;&c_i;&c_g;&c_je;&c_r;&c_i;&c_ja;</trd>
    <trd lng="de">Nigeria</trd>
    <trd lng="en">Nigeria</trd>
    <trdgrp lng="nl">
      <trd>Nigeria</trd>,
      <trd>Nigeri&euml;</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>Nigeria</trd>,
      <trd>Federalna Republika <ind>Nigerii</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

