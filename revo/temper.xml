<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: temper.xml,v 1.22 2023/10/21 13:02:26 revo Exp $">
    <kap>
      <ofc>1</ofc>
      <rad>temperament</rad>/o
    </kap>

    <drv mrk="temper.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Tuta korpa kaj spirita ecaro de homo konstatebla tra
          kutimaj humoroj kaj kondutoj:
          <ekz>
            agrabla, malmultekosta libro, ta&ubreve;ga por &ccirc;iuj
            a&gcirc;oj kaj &ccirc;iuj <tld/>oj
            <fnt>
          <aut>Paul Veenman</aut>,
          <vrk><url ref="&Monato;/1997/ffoj.html">Krimoj ... por &ccirc;iuj temperamentoj</url></vrk>,
          <bib>Monato</bib>, 
          <lok>1997/10, p. 19</lok>
        </fnt>.
          </ekz><ekz>
            <uzo tip="fak">PSI</uzo>
            la klasika tradicio distingas kvar bazajn <tld/>ojn:
            <refgrp tip="sub">
              <ref cel="flegm.0a">flegma</ref>,
              <ref cel="melank.0a">melankolia</ref>,
              <ref cel="kolerik.0a">kolerika</ref>,
              <ref cel="sangvi.0a">sangvina</ref>
            </refgrp>.
          </ekz>
        </dif>
      </snc>
      <refgrp tip="vid">
        <ref cel="karakt.0o">karaktero</ref>,
        <ref cel="person.0eco">personeco</ref>
      </refgrp>
      <trd lng="be">
      &c_t;&c_e;&c_m;&c_p;&c_e;&c_r;&c_a;&c_m;&c_e;&c_n;&c_t;</trd>
      <trd lng="de">Temperament</trd>
      <trd lng="en">temperament</trd>
      <trdgrp lng="fr">
        <trd>complexion</trd>,
        <trd>idiosyncrasie</trd>,
        <trd>temp&eacute;rament</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>v&eacute;rm&eacute;rs&eacute;klet</trd>,
        <trd>lelki alkat</trd>,
        <trd>temperamentum</trd>
      </trdgrp>
      <trdgrp lng="ja">
    <trd>&#x6C17;&#x8CEA;  <pr>&#x304D;&#x3057;&#x3064;</pr></trd>,
    <trd>&#x6C17;&#x6027;  <pr>&#x304D;&#x3057;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x6027;&#x5206;  <pr>&#x3057;&#x3087;&#x3046;&#x3076;&#x3093;</pr></trd>,
    <trd>&#x4F53;&#x8CEA;  <pr>&#x305F;&#x3044;&#x3057;&#x3064;</pr></trd>
  </trdgrp>
  <trd lng="nl">temperament</trd>
      <trd lng="pl">temperament</trd>
      <trd lng="sk">temperament</trd>
      <trd lng="pt">temperanto</trd>
      <trd lng="ru">
      &c_t;&c_je;&c_m;&c_p;&c_je;&c_r;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>
      <trdgrp lng="cs">
    <trd>letora</trd>,
    <trd>temperament</trd>
  </trdgrp>
  <trd lng="uk">&c_t;&c_je;&#x43C;&#x43F;&c_je;&c_r;&c_a;&#x43C;&c_je;&#x43D;&c_t;</trd>
</drv>
  </art>
