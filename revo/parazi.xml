<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>parazit</rad>/o
</kap>
<drv mrk="parazi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="parazi.0o.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      Besto a&ubreve; vegeta&jcirc;o, kiu vivas sur a&ubreve; en alia esta&jcirc;o,
      sin nutrante per &gcirc;iaj sukoj:
      <ekz>
        visko estas <tld/>o de kverko kaj pulo de homo.
      </ekz>
    </dif> 
   <trd lng="br">arvevad</trd>
    <trd lng="fr">parasite</trd>
  </snc>
  <snc mrk="parazi.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Nenifaranto, kiu vivas koste de aliaj homoj,
      profitas de ilia laboro,
      man&gcirc;as konstante &ccirc;e fremda tablo
      a&ubreve; simile.
    </dif>                         
   <trd lng="br">suner</trd>
   <trd lng="en">freeloader</trd>
    <trdgrp lng="fr">
      <trd>exploiteur</trd>,
      <trd>parasite<klr> (fig.)</klr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Parasit</trd>,
     <trd>Schmarotzer</trd>
  </trdgrp>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_z;&c_ib;&c_t;</trd>
  <trd lng="ca">par&agrave;sit</trd>
  <trd lng="en">parasite</trd>
  <trd lng="es">par&aacute;sito</trd>
  <trdgrp lng="hu">
    <trd>&eacute;l&odblac;sk&ouml;d&odblac;</trd>,
    <trd>parazita</trd>,
    <trd>&eacute;l&odblac;sdi</trd>
  </trdgrp>
  <trd lng="nl">parasiet</trd>
  <trd lng="pt">parasita</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_a;&c_z;&c_i;&c_t;</trd>
  <trd lng="sv">parasit</trd>
</drv>

<drv mrk="parazi.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Vivi <tld/>e je kostoj de aliaj.
    </dif>
  </snc>                             
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_z;&c_ib;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd> 
  <trdgrp lng="br">
    <trd>beva&ntilde; diwar-goust</trd>,
    <trd>sunata</trd>
  </trdgrp>
  <trd lng="de">schmarotzen</trd>
  <trd lng="es">parasitar</trd>
  <trd lng="fr">vivre en <ind>parasite</ind></trd>
  <trdgrp lng="en">
    <trd>leech (off of)</trd>,
    <trd>freeload</trd>
  </trdgrp>
  <trd lng="hu">&eacute;l&odblac;sk&ouml;dik</trd>    
  <trd lng="nl">parasiteren</trd>
  <trd lng="pt">parasitar</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_a;&c_z;&c_i;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
