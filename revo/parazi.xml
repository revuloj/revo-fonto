<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: parazi.xml,v 1.22 2021/05/03 08:57:20 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>parazit</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="parazi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="parazi.0o.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      Besto a&ubreve; vegeta&jcirc;o, kiu vivas sur a&ubreve; en alia 
      esta&jcirc;o, sin nutrante per &gcirc;iaj sukoj:
      <ekz>
        visko estas <tld/>o de kverko kaj pulo de homo.
      </ekz>
    </dif> 
   <trd lng="br">arvevad</trd>
   <trd lng="fr">parasite</trd>
   <trd lng="pl">paso&zdot;yt</trd>
  </snc>
  <snc mrk="parazi.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Nenifarulo, kiu vivas koste de aliaj homoj,
      profitas de ilia laboro,
      man&gcirc;as konstante &ccirc;e fremda tablo
      a&ubreve; simile.
    </dif>                         
   <trd lng="br">suner</trd>
   <trd lng="en">freeloader</trd>
    <trdgrp lng="fr">
      <trd>exploiteur</trd>,
      <trd>parasite<klr> (fig.)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_z;&c_ib;&c_t;</trd>
  <trd lng="ca">par&agrave;sit</trd>
  <trdgrp lng="cs">
    <trd>parazit</trd>,
    <trd>p&rcaron;&iacute;&zcaron;ivn&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Parasit</trd>,
     <trd>Schmarotzer</trd>
  </trdgrp>
  <trd lng="en">parasite</trd>
  <trd lng="es">par&aacute;sito</trd>
  <trdgrp lng="hu">
    <trd>&eacute;l&odblac;sk&ouml;d&odblac;</trd>,
    <trd>parazita</trd>,
    <trd>&eacute;l&odblac;sdi</trd>
  </trdgrp>
  <trd lng="nl">parasiet</trd>
  <trdgrp lng="pl">
    <trd>darmozjad</trd>,
    <trd>paso&zdot;yt</trd>
  </trdgrp>
  <trd lng="pt">parasita</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_a;&c_z;&c_i;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>cudzopasn&iacute;k</trd>,
    <trd>pr&iacute;&zcaron;ivn&iacute;k</trd>
  </trdgrp>
  <trd lng="sv">parasit</trd>
</drv>

<drv mrk="parazi.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="parazi.0i.EKOL">
    <dif>
      Vivi <tld/>e &ccirc;e alia vivulo:
      <ekz>
        oni memoru anka&ubreve; la viskon <klr>[&#8230;]</klr>, kiu iumaniere kvaza&ubreve;grefti&gcirc;as, sur la
        arboj genetike tre malproksimaj, kiujn &gcirc;i <tld/>as
        <fnt>
          <aut>C. Roux</aut>
          <vrk><url ref="http://www.eventoj.hu/steb/agrikulturo/greftado/greftado.htm"
              >Greftado</url></vrk>
          <lok>La Domo, 1997-01 (13)</lok>
        </fnt>;
      </ekz>
      <ekz>
        el la iksodoj &gcirc;enerale la &scirc;ildhavaj <nac>Ixodidae</nac>
        parazitas en reptilioj, malpli ofte en amfibioj
        <fnt>
          <aut>U. Zillmann kaj F. Mutschmann, trad. R. Sachs</aut>
          <vrk><url
          ref="http://www.eventoj.hu/steb/zoologio/kompendio-parazitoj-de-bestoj/index-kompendio-parazitoj-de-bestoj.">
          Kompendio de la plej gravaj parazitoj &ccirc;e malgrandaj kaj laboratoriaj bestoj</url></vrk>
          <lok>Scienca Revuo, kromkajero, 2000</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="parazi.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Vivi <tld/>e je kostoj de aliaj homoj.
    </dif>
  </snc>
  <trd lng="be">
  &c_p;&c_a;&c_r;&c_a;&c_z;&c_ib;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="br">
    <trd>beva&ntilde; diwar-goust</trd>,
    <trd>sunata</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>cizopasit</trd>,
    <trd>parazitovat</trd>,
    <trd>p&rcaron;i&zcaron;ivovat se</trd>,
    <trd>&zcaron;&iacute;t p&rcaron;&iacute;&zcaron;ivnicky</trd>
  </trdgrp>
  <trd lng="de">schmarotzen</trd>
  <trdgrp lng="en">
    <trd>leech (off of)</trd>,
    <trd>freeload</trd>
  </trdgrp>
  <trd lng="es">parasitar</trd>
  <trd lng="fr">parasiter</trd>
  <trd lng="hu">&eacute;l&odblac;sk&ouml;dik</trd>
  <trd lng="nl">parasiteren</trd>
  <trd lng="pl">paso&zdot;ytowa&cacute;</trd>
  <trd lng="pt">parasitar</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_a;&c_z;&c_i;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>cudzopasi&tcaron;</trd>,
    <trd>pri&zcaron;ivova&tcaron; sa</trd>
  </trdgrp>
</drv>

<drv mrk="parazi.0ato">
  <kap>
    <tld/>ato
  </kap>
  <snc mrk="parazi.0ato.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      Vivulo sur a&ubreve; en kiu vivas <tld/>o;
      <ref tip="sin" cel="gast.0iganto.EKOL">gastiganto</ref>:
      <ekz>
        vundoj <klr>[&#8230;]</klr> pro batalo inter la <tld/>o kaj la
        <tld/>ato, ofte i&gcirc;as pli-malpli tuberoformaj
        <fnt>
          <bib>PIV1</bib>
          <lok>artikolo &leftquot;kancero&rightquot;</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">h&ocirc;te<klr> (d'un parasite)</klr></trd>
</drv>

<drv mrk="parazi.0ismo">
  <kap><tld/>ismo,
  <var><kap><tld/>eco</kap></var></kap>
  <snc mrk="parazi.0ismo.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      <tld lit="P"/>a moro:
      <ekz>
        la sudamerika heteroneto <klr>[&#8230;]</klr> adapti&gcirc;is 
        al konstanta <tld/>ismo &ccirc;e aliaj na&gcirc;antoj &ndash;
        laroj kaj akvolaroj
        <fnt>
          <aut>J. &Scirc;uvajev</aut>,
          <vrk><url
          ref="http://www.eventoj.hu/steb/zoologio/konduto_de_bestoj/f874-p30.htm">
          Pri la konduto de bestoj</url></vrk>,
          <lok>Fokuso, 1987:1, p. 2a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_z;&c_ib;&c_t;&c_y;&c_z;&c_m;</trd>
  <trd lng="cs">p&rcaron;&iacute;&zcaron;ivnictv&iacute;</trd>
  <trd lng="fr">parasitisme</trd>
  <trd lng="sk">pr&iacute;&zcaron;ivn&iacute;ctvo</trd>
</drv>

<drv mrk="parazi.ekstera_0o">
  <kap>
    ekstera <tld/>o
  </kap>
  <snc mrk="parazi.ekstera_0o.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      <tld lit="P"/>o vivanta ekstere de la korpo de la gastiganto:
      <ekz>
        je la preferataj korporegionoj de eksteraj <tld/>oj la felo estu kuspe
        premkovrata per la gluflanko de <klr>[&#8230;]</klr> glueca bendo
        <fnt>
          <aut>U. Zillmann kaj F. Mutschmann, trad. R. Sachs</aut>
          <vrk><url
          ref="http://www.eventoj.hu/steb/zoologio/kompendio-parazitoj-de-bestoj/index-kompendio-parazitoj-de-bestoj.">
          Kompendio de la plej gravaj parazitoj &ccirc;e malgrandaj kaj laboratoriaj bestoj</url></vrk>
          <lok>Scienca Revuo, kromkajero, 2000</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">ectoparasite</trd>
</drv>

<drv mrk="parazi.interna_0o">
  <kap>
    interna <tld/>o
  </kap>
  <snc mrk="parazi.interna_0o.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      <tld lit="P"/>o vivanta en la korpo de la gastiganto:
      <ekz>
        kiel aliaj kantobirdoj <klr>[kantoturdo]</klr> estas atakata de eksteraj kaj
        internaj <tld/>oj kaj estas predata de katoj kaj rabobirdoj
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Kantoturdo"
              >Kantoturdo</url></vrk>
          <lok>2020-02-19</lok>
        </fnt>;
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">endoparasite</trd>
</drv>
</art>
