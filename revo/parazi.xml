<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>parazit</rad>/o
</kap>
<drv mrk="parazi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="parazi.0o.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      Besto a&ubreve; vegeta&jcirc;o, kiu vivas sur a&ubreve; en alia esta&jcirc;o,
      sin nutrante per &gcirc;iaj sukoj:
      <ekz>
        visko estas <tld/>o de kverko kaj pulo de homo.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>parasite</trd>
    </trdgrp>
  </snc>
  <snc mrk="parazi.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Nenifaranto, kiu vivas koste de aliaj homoj,
      profitas de ilia laboro,
      man&gcirc;as konstante &ccirc;e fremda tablo
      a&ubreve; simile.
    </dif>
    <trdgrp lng="fr">
      <trd>exploiteur</trd>,
      <trd>parasite<klr> (fig.)</klr></trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Parasit</trd>,
     <trd>Schmarotzer</trd>
  </trdgrp>
  <trd lng="nl">parasiet</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_a;&c_z;&c_i;&c_t;</trd>
  <trd lng="sv">parasit</trd>
</drv>

<drv mrk="parazi.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Vivi <tld/>e je kostoj de aliaj.
    </dif>
  </snc>
  <trd lng="de">schmarotzen</trd>
  <trdgrp lng="fr">
    <trd>vivre en <ind>parasite</ind></trd>
  </trdgrp>
  <trd lng="nl">parasiteren</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_a;&c_z;&c_i;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
