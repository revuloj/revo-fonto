<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>parazit</rad>/o
</kap>
<drv mrk="parazi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Tiu, kiu kvaza&ubreve; profesie man&gcirc;as konstante &ccirc;e fremda tablo.
    </dif>
  </snc>
  <snc>
    <dif>
      Nenifaranto, kiu ekspluatas la laboron de aliaj.
    </dif>
  </snc>
  <snc>
    <dif>
      Besto a&ubreve; veg., kiu vivas sur alia esta&jcirc;o, sin nutrante per &gcirc;iaj
      sukoj:
      <ekz>
        visko estas <tld/>o de kverko kaj pulo de homo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Parasit</trd>
  <trd lng="nl">parasiet</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_a;&c_z;&c_i;&c_t;</trd>
  <trd lng="sv">parasit</trd>
</drv>

<drv mrk="parazi.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Vivi <tld/>e je kostoj de aliaj.
    </dif>
  </snc>
  <trd lng="de">schmarotzen</trd>
  <trd lng="nl">parasiteren</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_a;&c_z;&c_i;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
