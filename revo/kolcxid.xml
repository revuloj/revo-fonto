    <drv mrk="kolcxid.K0o">
        <kap><tld/>o</kap>
        <snc>
        <uzo tip="fak">HIS</uzo>
        <uzo tip="fak">GEOG</uzo>
        <uzo tip="stl">EVI</uzo>
            <dif>
                <ref tip="dif" cel="kolhxid.K0o">Kol&hcirc;ido</ref>
