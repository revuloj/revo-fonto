<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Hilbert</rad>/o
</kap>
<drv mrk="hilber.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <dif>
      Germanlingve: David <trd lng="de">Hilbert</trd>, 1862-1943.
      Germana matematikisto.
    </dif>
  </snc>
  <trd lng="en">Hilbert</trd>
  <trd lng="fr">Hilbert</trd>
  <trd lng="hu">Hilbert</trd>
  <trd lng="pl">Hilbert</trd>
  <trd lng="ru">&c_G;&c_i;&c_l;&c_mol;&c_b;&c_je;&c_r;&c_t;</trd>
</drv>

<drv mrk="hilber.h0a">
  <kap><tld lit="h"/>a</kap>
  <fnt><bib>Deneva</bib></fnt>
  <snc>
    <refgrp tip="vid">
      <ref cel="spac.hilberta0o"><tld lit="h"/>a spaco</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Hilbertsch</trd>
  <trd lng="en">Hilbert['s]</trd>
  <trdgrp lng="fr">
    <trd>de <ind>Hilbert</ind></trd>,
    <trd>hilbertien</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>Hilbert-</trd>,
    <trd>Hilbert-f&eacute;le</trd>
  </trdgrp>
  <trd lng="pl">Hilberta</trd>
  <trdgrp lng="ru">
    <trd>&c_G;&c_i;&c_l;&c_mol;&c_b;&c_je;&c_r;&c_t;&c_a;</trd>,
    <trd>&c_g;&c_i;&c_l;&c_mol;&c_b;&c_je;&c_r;&c_t;&c_o;&c_v;</trd></trdgrp> 
  </drv>
</art>

