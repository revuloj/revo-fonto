<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hilber.xml,v 1.11 2023/04/21 09:26:06 revo Exp $">
<kap>
  <rad>Hilbert</rad>/o
  <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="hilber.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Germanlingve: <nom>David</nom> <trd lng="de">Hilbert</trd>, 1862-1943;
      germana <ref tip="lst" lst="voko:historiaj_personoj" cel="matema.0isto">matematikisto</ref>:
      <ekz>
        la <tld/>a listo de problemoj <klr>[formulita]</klr> en 1900 de Davido <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=87417"
              >Hilberta kvina problemo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_G;&c_ib;&c_l;&c_mol;&c_b;&c_e;&c_r;&c_t;</trd>
  <trd lng="cs">Hilbert David</trd>
  <trd lng="en">Hilbert</trd>
  <trd lng="fr">Hilbert</trd>
  <trd lng="hu">Hilbert</trd>
  <trd lng="it">Hilbert</trd>
  <trd lng="pl">Hilbert</trd>
  <trd lng="pt">Hilbert</trd>
  <trd lng="ru">&c_G;&c_i;&c_l;&c_mol;&c_b;&c_je;&c_r;&c_t;</trd>
  <trd lng="sk">Hilbert David</trd>
</drv>

<drv mrk="hilber.h0a">
  <kap><tld lit="h"/>a</kap>
  <fnt><bib>Deneva</bib></fnt>
  <snc>
    <dif>
      Matematika nocio nomita la&ubreve; <tld/>o:
      <ekz>
        en matematiko, <tld lit="h"/>a sterna&jcirc;o estas sterna&jcirc;o modelita en <tld
        lit="h"/>a spaco, tial &gcirc;i estas topologia spaco en kiu &ccirc;iu punkto havas
        najbarecon homeomorfian al <tld lit="h"/>a spaco
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=83260"
              >Hilberta sterna&jcirc;o</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="spac.hilberta0o"><tld lit="h"/>a spaco</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_G;&c_ib;&c_l;&c_mol;&c_b;&c_e;&c_r;&c_t;&c_a;</trd>,
    <trd>&c_g;&c_ib;&c_l;&c_mol;&c_b;&c_e;&c_r;&c_t;&c_a;&c_w;</trd>
  </trdgrp>
  <trd lng="de">Hilbertsch</trd>
  <trd lng="en">Hilbert['s]</trd>
  <trdgrp lng="fr">
    <trd>de <ind>Hilbert</ind></trd>,
    <trd>hilbertien</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>Hilbert-</trd>,
    <trd>Hilbert-f&eacute;le</trd>
  </trdgrp>
  <trd lng="it">hilbertiano</trd>
  <trd lng="pl">Hilberta</trd>
  <trdgrp lng="ru">
    <trd>&c_G;&c_i;&c_l;&c_mol;&c_b;&c_je;&c_r;&c_t;&c_a;</trd>,
    <trd>&c_g;&c_i;&c_l;&c_mol;&c_b;&c_je;&c_r;&c_t;&c_o;&c_v;</trd></trdgrp> 
  </drv>
</art>

