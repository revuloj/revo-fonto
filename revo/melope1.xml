<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: melope1.xml,v 1.15 2020/08/09 22:10:34 revo Exp $">
<kap>
  <rad>melopep</rad>/o
</kap>
<drv mrk="melope1.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_specioj" cel="speci.0o.BIO">Specio</ref>
      de <ref tip="super" cel="kukurb.0o">kukurbo</ref>
      (<trd lng="la">Cucurbita melopepo</trd> sin. <trd lng="la">Cucurbita pepo</trd>),
      resp. ties frukto:
      <ekz>
        la kukurbo, alivorte <tld/>o, devenas el la Meza Ameriko kaj &gcirc;ia historio
        atingas la antikvecon
        <fnt>
          <vrk><url ref="https://komentoj.blogspot.com/#!"
              >Miaj Komentoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        skribita sur putri&gcirc;anta <tld/>o
        <fnt>
          <aut>Robert Patrick</aut>
          <vrk><url ref="https://eo.wikiquote.org/wiki/Robert_Patrick"
              >Vikicitaro</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/d/d1/2006-10-18Cucurbita_pepo02.jpg/640px-2006-10-18Cucurbita_pepo02.jpg"
      prm="CC BY 2.5"
      >kelkaj <tld/>oj
        <fnt>
          <aut>Wildfeuer, propra foto</aut>
          <lok><url ref="&WCW;/File:2006-10-18Cucurbita_pepo02.jpg"
              >https://commons.wikimedia.org/wiki/File:2006-10-18Cucurbita_pepo02.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="de">Gartenk&uuml;rbis</trd>
  <trdgrp lng="en">
    <trd>squash</trd>,
    <trd>summer squash</trd>,
    <trd>yellow squash</trd>,
    <trd>spaghetti squash</trd>
  </trdgrp>
  <trd lng="fr">courge spaghetti</trd>
  <trd lng="hu">koron&aacute;s <ind>t&ouml;k</ind></trd>
  <trd lng="nl">tulbandkalebas</trd>
  <trdgrp lng="pl">
    <trd>dynia</trd>,
    <trd>baniomelon</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>ab&oacute;bora</trd>,
    <trd>jerimum</trd>
  </trdgrp>
  <trd lng="ro">dovlecel</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_t;&c_i;&c_s;&c_s;&c_o;&c_n;</trd>,
    <trd>&c_t;&c_a;&c_r;&c_je;&c_l;&c_mol;&c_ch;&c_a;&c_t;&c_a;&c_ja; &c_t;&c_y;&c_k;&c_v;&c_a;</trd>
  </trdgrp>
</drv>
</art>
