<vortaro>
<art mrk="$Id$">
<kap>
  <rad>melopep</rad>/o
</kap>
<drv mrk="melope1.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="kukurb.0o">kukurbo</ref>
      (<trd lng="la">Cucurbita melopepo</trd>), kulturata
      en Ameriko.
    </dif>
  </snc>
  <trd lng="de">Turbankürbis</trd>
  <trdgrp lng="en">
    <trd>squash</trd>,
    <trd>summer squash</trd>,
    <trd>yellow squash</trd>,
    <trd>spaghetti squash</trd>
  </trdgrp>
  <trd lng="hu">koronás <ind>tök</ind></trd>
  <trd lng="nl">tulbandkalebas</trd>
  <trdgrp lng="pt">
    <trd>abóbora</trd>,
    <trd>jerimum</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>патиссон</trd>,
    <trd>тарельчатая
тыква</trd>
  </trdgrp>
</drv>
</art>
