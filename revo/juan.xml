<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: juan.xml,v 1.15 2022/03/21 17:23:46 revo Exp $">
<kap>
  <rad>juan</rad>/o <fnt><bib>SPIV</bib></fnt>
</kap>
<drv mrk="juan.0o">
  <kap><tld/>o</kap>
  <mlg>CNY</mlg>
  <mlg>&#x4B0;</mlg>
  <mlg>&#x5143;</mlg>
  <snc mrk="juan.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="lst" cel="unu.mon0o.EKON" lst="voko:monunuoj">Monunuo</ref> 
      de &Ccirc;inujo:
      <ekz>
        la onklino ricevos nur akciojn valorantajn kvincent <tld/>ojn
        <fnt><bib>LaFamilio</bib>, <lok>&ccirc;apitro 35a, p. 505a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>iuan</trd>,
    <trd>renminbi</trd>
  </trdgrp>
  <trd lng="cs">juan</trd>
  <trd lng="de">Yuan</trd>
  <trd lng="en">yuan</trd>
  <trdgrp lng="fr">
    <trd>yuan<klr> (monnaie chinoise)</klr></trd>,
    <trd>renminbi</trd>
  </trdgrp>
  <trd lng="id">yuan</trd>
  <trd lng="it">yuan</trd>
  <trd lng="pl">juan</trd>
  <trdgrp lng="pt">
    <trd>iuane</trd>,
    <trd>yuan</trd>
  </trdgrp>
  <trd lng="sk">juan</trd>
  <trd lng="uk">&#x44E;&#x430;&#x43D;&#x44C; (&#x43A;&#x438;&#x442;&#x430;&#x439;&#x441;&#x44C;&#x43A;&#x430; &#x433;&#x440;&#x43E;&#x448;&#x43E;&#x432;&#x430; &#x43E;&#x434;&#x438;&#x43D;&#x438;&#x446;&#x44F;)</trd>
  <trd lng="zh">&#x5143; [<ind>yu&aacute;n</ind>]</trd>
</drv>
</art>

