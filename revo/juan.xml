<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: juan.xml,v 1.16 2022/04/30 18:36:00 revo Exp $">
<kap>
  <rad>juan</rad>/o <fnt><bib>SPIV</bib></fnt>
</kap>
<drv mrk="juan.0o">
  <kap><tld/>o</kap>
  <mlg>CNY</mlg>
  <mlg>&#x4B0;</mlg>
  <mlg>&#20803;</mlg>
  <snc mrk="juan.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="lst" cel="unu.mon0o.EKON" lst="voko:monunuoj">Monunuo</ref> 
      de &Ccirc;inujo:
      <ekz>
        la onklino ricevos nur akciojn valorantajn kvincent <tld/>ojn
        <fnt><bib>LaFamilio</bib>, <lok>&ccirc;apitro 35a, p. 505a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ju;&c_a;&c_n;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>iuan</trd>,
    <trd>renminbi</trd>
  </trdgrp>
  <trd lng="cs">juan</trd>
  <trd lng="de">Yuan</trd>
  <trd lng="en">yuan</trd>
  <trdgrp lng="fr">
    <trd>yuan<klr> (monnaie chinoise)</klr></trd>,
    <trd>renminbi</trd>
  </trdgrp>
  <trd lng="id">yuan</trd>
  <trd lng="it">yuan</trd>
  <trd lng="pl">juan</trd>
  <trdgrp lng="pt">
    <trd>iuane</trd>,
    <trd>yuan</trd>
  </trdgrp>
  <trd lng="sk">juan</trd>
  <trd lng="uk">&#x44E;&c_a;&#x43D;&#x44C; (&#x43A;&c_i;&c_t;&c_a;&c_j;&c_s;&#x44C;&#x43A;&c_a; &c_g;&c_r;&#x43E;&c_sh;&#x43E;&c_v;&c_a;
  &#x43E;&c_d;&c_i;&#x43D;&c_i;&c_c;&#x44F;)</trd>
  <trd lng="zh">&#20803; [<ind>yu&aacute;n</ind>]</trd>
</drv>
</art>

