<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>juan</rad>/o <fnt><bib>SPIV</bib></fnt>
</kap>
<drv mrk="juan.0o">
  <kap><tld/>o</kap>
  <snc mrk="juan.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="unu.mon0o.EKON">Monunuo</ref> 
      de &Ccirc;inujo:
      <ekz>
        la onklino ricevos nur akciojn valorantajn kvincent <tld/>ojn
        <fnt><bib>LaFamilio</bib>, <lok>&ccirc;apitro 35a, p. 505a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>iuan</trd>,
    <trd>renminbi</trd>
  </trdgrp>
  <trd lng="de">Yuan</trd>
  <trd lng="en">yuan</trd>
  <trd lng="fr">yuan<klr tip="amb"> (monnaie chinoise)</klr></trd>
  <trd lng="pl">juan</trd>
  <trdgrp lng="pt">
    <trd>iuane</trd>,
    <trd>yuan</trd>
  </trdgrp>
  <trd lng="it">yuan</trd>
</drv>
</art>

