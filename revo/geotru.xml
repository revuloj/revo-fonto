<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: geotru.xml,v 1.21 2022/03/19 16:47:06 revo Exp $">
<kap>
  <rad>geotrup</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="geotru.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="geotru.0o.ZOO">
    <dif>
      Scienca genra nomo de <ref tip="dif" cel="skarab.sterko0o.ZOO">sterkoskarabo</ref>:
      <ekz>
        <tld/>o, latine &leftquot;<nom>Geotrupes</nom>&rightquot;, signifas &leftquot;terboranto&rightquot;.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_n;&c_a;&c_ja;&c_v;&c_ib;&c_k;</trd>
  <trd lng="bg">&c_t;&c_o;&c_r; &c_b;&c_r;&c_malmol;&c_m;&c_b;&c_a;&c_r;</trd>
  <trd lng="cs">chrob&aacute;k velk&yacute;</trd>
  <trd lng="de">Mistk&auml;fer</trd>
  <trdgrp lng="en">
    <trd>earth-boring dung beetle</trd>,
    <trd>dor beetle</trd>
  </trdgrp>
  <trd lng="fr">g&eacute;otrupe</trd>
  <trd lng="hu">ganajt&uacute;r&oacute; bog&aacute;r</trd>
  <trd lng="nl">mestkever</trd>
  <trd lng="pt">escaravelho</trd>
  <trd lng="ru">&c_n;&c_a;&c_v;&c_o;&c_z;&c_n;&c_y;&c_j; &c_zh;&c_u;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>hnojival</trd>,
    <trd>lajniak</trd>
  </trdgrp>
  <trd lng="uk">&#x433;&#x43D;&#x43E;&#x439;&#x43E;&#x432;&#x438;&#x43A;</trd>
</drv>

</art>
