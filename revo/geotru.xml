<vortaro>
<art mrk="$Id$">
<kap>
  <rad>geotrup</rad>/o
</kap>
<drv mrk="geotru.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super"
      cel="geotru1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Geotrupes</trd>),
      nigre, blue a&ubreve; verde brilanta kaj
      vivanta en sterka&jcirc;oj.
    </dif>
    <ref tip="sin" cel="skarab.sterko0o">sterkoskarabo</ref>
  </snc>
  <trd lng="de">Mistk&auml;fer</trd>
  <trdgrp lng="en">
    <trd>earth-boring dung beetle</trd>,
    <trd>dor beetle</trd>
  </trdgrp>
  <trd lng="fr">g&eacute;otrupe</trd>
  <trd lng="hu">ganajt&uacute;r&oacute; bog&aacute;r</trd>
  <trd lng="nl">mestkever</trd>
  <trd lng="pt">escaravelho</trd>
  <trd lng="ru">&c_n;&c_a;&c_v;&c_o;&c_z;&c_n;&c_y;&c_j; &c_zh;&c_u;&c_k;</trd>
</drv>

</art>
