<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>geotrup</rad>/o
</kap>
<drv mrk="geotru.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="geotru.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Geotrupes</trd>),
      nigre, blue a&ubreve; verde brilanta kaj
      vivanta en sterka&jcirc;oj.
    </dif>
    <ref tip="super" cel="skarab.sterko0o">sterkoskarabo</ref>
  </snc>
  <trd lng="de">Mistk&auml;fer</trd>
</drv>
<drv mrk="geotru.0edoj">
  <kap><tld/>edoj</kap>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="koleop.0oj">koleopteroj</ref>
      (<trd lng="la">Geotrupidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Mistk&auml;fer <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>
