<drv mrk="ebl.0e">
  <kap><ofc>*</ofc><tld/>e <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="&AdE;/fundamento/universala_vortaro.html#av-5010">ebl'e</url></lok></fnt></kap>
  <snc>
    <dif>
      Eventuale, okazeble, povas esti ke:
      <ekz>
        <tld/>e mi havos infanojn
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 16:2</lok>
        </fnt>;
      </ekz><ekz>
        <tld/>e mi mortos pro &scirc;i
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 26:9</lok>
        </fnt>;
      </ekz><ekz>
        <tld/>e li akceptos min favore
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 32:20</lok>
        </fnt>;
      </ekz><ekz>
        mi blinde pafos, <tld/>e trafos
        <fnt><bib>PrV</bib></fnt>;
      </ekz><ekz>
        faru hodia&ubreve;, kion vi povas &dash; morga&ubreve; vi <tld/>e okazon ne trovos
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="af">miskien</trd>
  <trd lng="am">&#x121D;&#x1293;&#x120D;&#x1263;&#x1275;</trd>
  <trd lng="ar">&a_r;&a_b;&a_m;&a_A;</trd>
  <trd lng="az">b&#x259;lk&#x259;</trd>
  <trd lng="be">&c_m;&c_a;&c_g;&c_ch;&c_y;&c_m;&c_a;</trd>
  <trd lng="bn">&#x9B8;&#x9AE;&#x9CD;&#x9AD;&#x9AC;&#x9A4;</trd>
  <trd lng="bo">&#xF42;&#xF45;&#xF72;&#xF42;&#xF0B;&#xF56;&#xFB1;&#xF66;&#xF0B;&#xF53;&#xF0B;</trd>
  <trd lng="bs">mo&#x17E;da</trd>
  <trd lng="ca">potser</trd>
  <trd lng="co">forsi</trd>
  <trdgrp lng="cs">
    <trd>asi <klr>(snad)</klr></trd>,
    <trd>mo&zcaron;no</trd>,
    <trd>mo&zcaron;n&aacute;</trd>
  </trdgrp>
  <trd lng="cy">efallai</trd>
  <trd lng="da">m&#xE5;ske</trd>
  <trdgrp lng="de">
    <trd>vielleicht</trd>,
    <trd>m&ouml;glicherweise</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>possibly</trd>,
    <trd>maybe</trd>,
    <trd>perhaps</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>posiblemente</trd>,
    <trd>quiz&aacute;</trd>
  </trdgrp>
