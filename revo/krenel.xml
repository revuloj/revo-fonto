<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: krenel.xml,v 1.21 2023/12/03 09:59:00 revo Exp $">
<kap>
<rad>krenel</rad>/o
</kap>
<drv mrk="krenel.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Dentoforma aperturo farita &ccirc;e la supro de muro tra kiu oni 
pafas.
    </dif>
    <trd lng="be">&c_k;&c_r;&c_e;&c_n;&c_e;&c_l;&c_mol;</trd>
    <trd lng="de">Schie&szlig;scharte</trd>
    <trd lng="en">crenel</trd>
    <trd lng="es">almena</trd> 
    <trd lng="hu">l&odblac;r&eacute;s</trd>
    <trd lng="pl">blanki {pl}</trd>
  </snc>
  <snc>
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Dentita masona&jcirc;o &ccirc;e la supro de turo, citadelo:
      <ekz>la majestaj <tld/>oj tuj faras grandan impreson al la okuloj
        <fnt>
          <aut>Charlotte Bront&euml;, trad. H. J. Bulthuis</aut>
          <vrk>Jane Eyre, Dua parto</vrk>
          <lok>&ccirc;apitro 36a</lok>
        </fnt>;
      </ekz>
      <ekz>vulturoj kaj korvoj kiuj flugilfrape ekflugas &ccirc;irka&ubreve; la
      palaco, eksidas &ccirc;ie, sur kupoloj, <tld/>oj kaj turoj
        <fnt>
          <aut>Artur Lundkvist, trad. Bertil Nilsson</aut>
          <vrk>La volo de l'&ccirc;ielo</vrk>
        </fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="embraz.0">embrazuro</ref>.
    </refgrp>
    <trd lng="be">&c_k;&c_r;&c_e;&c_n;&c_e;&c_l;&c_ja;&c_zh;</trd>
    <trd lng="de">Zinne</trd>
    <trd lng="en">crenellation</trd>
    <trdgrp lng="hu">
      <trd>p&aacute;rtakorona</trd>,
      <trd>oromcsipk&eacute;zet</trd>
    </trdgrp>
    <trd lng="pl">krenela&zdot;</trd>
  </snc>
  <trd lng="fr">cr&eacute;neau</trd>
  <trdgrp lng="it">
    <trd>dente<klr> (arc. mil.)</klr></trd>,
    <trd>merlo<klr> (arc. mil.)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x9283;&#x773C;  <pr>&#x3058;&#x3085;&#x3046;&#x304C;&#x3093;</pr></trd>
  <trd lng="nl">kanteel</trd>
  <trd lng="pt">ameia</trd>
  <trdgrp lng="ro">
    <trd>merlon</trd>,
    <trd>creneluri</trd>
  </trdgrp> 
  <trd lng="ru">&c_z;&c_u;&c_b;&c_je;&c_c; &c_s;&c_t;&c_je;&c_n;&c_y;</trd>
  <trd lng="uk">&c_z;&c_u;&c_b;&c_je;&c_c;&#x44C; (&c_s;&c_t;&c_ib;&#x43D;&c_i;)</trd>
  <trdgrp lng="zh">
    <trd>&#x579B;&#x53E3; <pr>du&#x1D2;k&#x1D2;u</pr></trd>,
    <trd>&#x581E; <pr>di&#xE9;</pr></trd>,
    <trd>&#x57CE;&#x579B; <pr>ch&#xE9;ngdu&#x1D2;</pr></trd>,
    <trd>&#x5973;&#x5899; <pr>n&#x1DA;qi&#xE1;ng</pr></trd>,
    <trd>&#x5973;&#x7246; <pr>n&#x1DA;qi&#xE1;ng</pr></trd>,
    <trd>&#x9632;&#x536B;&#x5899; <pr>f&#xE1;ngw&#xE8;iqi&#xE1;ng</pr></trd>,
    <trd>&#x9632;&#x885B;&#x7246; <pr>f&#xE1;ngw&#xE8;iqi&#xE1;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="krenel.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>ojn:
      <ekz>
        <tld/>i muron.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="brecx">bre&ccirc;eti</ref>,
      <ref cel="dent">denti</ref>,
      <ref cel="feston">festoni</ref>.
    </refgrp>
    <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_z;&c_u;&c_b;&c_c;&c_y;</ind> <klr>(&c_u; &c_m;&c_u;&c_r;&c_y;)</klr></trd>
    <trdgrp lng="de">
      <trd>mit <ind>Schie&szlig;scharten versehen</ind></trd>,
      <trd>krenelieren</trd>
    </trdgrp>
    <trd lng="en">crenellate</trd>
    <trd lng="fr">cr&eacute;neler</trd>
    <trd lng="hu">csipk&eacute;z<klr tip="amb"> (oromzatot)</klr></trd>
    <trdgrp lng="it">
      <trd>merlare</trd>, 
      <trd>dentellare</trd>
    </trdgrp>
    <trd lng="nl">van <ind>kantelen</ind> voorzien<klr tip="ind">
    (daad)</klr></trd>
    <trd lng="pl">z&aogonek;bkowa&cacute;</trd>     
    <trd lng="ro">i da din&#x21B;ii</trd>
  </snc>
  <trd lng="ja">&#x9283;&#x773C;&#x3092;&#x3064;&#x3051;&#x308B;  <pr>&#x3058;&#x3085;&#x3046;&#x304C;&#x3093;&#x3092;&#x3064;&#x3051;&#x308B;</pr></trd>
</drv>

<drv mrk="krenel.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Havanta <tld/>ojn:
      <ekz>
        <tld/>a muro;
      </ekz>
      <ekz>la gardistoturojn kun iliaj <tld/>aj suproj kaj fendoj por
      pafarkistoj
        <fnt>
          <aut>Artur Lundkvist, trad. Bertil Nilsson</aut>
          <vrk>La volo de l'&ccirc;ielo</vrk>
        </fnt>
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>mit <ind>Schie&szlig;scharten versehen</ind></trd>,
      <trd>mit <ind>Zinnen versehen</ind></trd>,
      <trd>kreneliert</trd>
    </trdgrp>
    <trd lng="en">crenellated</trd>
    <trdgrp lng="es">
      <trd>almenada</trd>,
      <trd>almenado</trd>
    </trdgrp> 
    <trd lng="hu">csipk&eacute;zett</trd>
    <trdgrp lng="it">
      <trd>dentellato<klr> (arc. mil.)</klr></trd>,
      <trd>merlato<klr> (arc. mil.)</klr></trd>
    </trdgrp>
    <trd lng="nl">van <ind>kantelen</ind> voorzien<klr tip="ind">
    (resultaat)</klr></trd>
    <trd lng="ru">&c_z;&c_u;&c_b;&c_ch;&c_a;&c_t;&c_y;&c_j;</trd>
  </snc>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Havanta tran&ccirc;ojn similajn al <tld/>o:
      <ekz>
        <tld/>aj folioj.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>gezackt</trd>,
      <trd>gezahnt</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>crenate</trd>,
      <trd>notched</trd>,
      <trd>scalloped</trd>,
      <trd>toothed</trd>
    </trdgrp>
    <trd lng="hu">csipk&eacute;s sz&eacute;l&udblac;</trd>
    <trdgrp lng="it">
    <trd>dentellato<klr> (bot.)</klr></trd>,
    <trd>smerlato<klr> (bot.)</klr></trd>,
    <trd>crenato<klr> (bot.)</klr></trd>
  </trdgrp>
    <trd lng="nl">gekarteld</trd>
    <trdgrp lng="ru">
      <trd>&c_z;&c_u;&c_b;&c_ch;&c_a;&c_t;&c_y;&c_j;</trd>,
      <trd>&c_z;&c_a;&c_z;&c_u;&c_b;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_z;&c_u;&c_b;&c_ch;&c_a;&c_s;&c_t;&c_y;</trd>
    <trd lng="fr">cr&eacute;nel&eacute;</trd>
    <trdgrp lng="ja">
    <trd>&#x9283;&#x773C;&#x306E;&#x3064;&#x3044;&#x305F;  <pr>&#x3058;&#x3085;&#x3046;&#x304C;&#x3093;&#x306E;&#x3064;&#x3044;&#x305F;</pr></trd>,
    <trd>&#x920D;&#x92F8;&#x6B6F;&#x72B6;&#x306E;  <pr>&#x3069;&#x3093;&#x92F8;&#x6B6F;&#x3058;&#x3087;&#x3046;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="pl">z&eogonek;baty</trd>     
    <trd lng="ro">din&#x21B;at</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_u;&c_b;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_u;&c_b;&c_ch;&c_a;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x51F9;&#x51F8;&#x5F62; <pr>&#x101;ot&#x16B;x&#xED;ng</pr></trd>,
    <trd>&#x6709;&#x9F52;&#x72C0;&#x7A81;&#x8D77; <pr>y&#x1D2;uch&#x1D0;zhu&#xE0;ngt&#x16B;q&#x1D0;</pr></trd>,
    <trd>&#x6709;&#x9F7F;&#x72B6;&#x7A81;&#x8D77; <pr>y&#x1D2;uch&#x1D0;zhu&#xE0;ngt&#x16B;q&#x1D0;</pr></trd>
  </trdgrp>
</drv>
</art>
