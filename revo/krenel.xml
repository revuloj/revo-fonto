<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>krenel</rad>/o
</kap>
<drv mrk="krenel.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Dentoforma aperturo farita &ccirc;e la supro de muro tra kiu oni pafas.
    </dif>
    <trd lng="en">crenel</trd>
  </snc>
  <snc>
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Dentita masona&jcirc;o &ccirc;e la supro de turo, citadelo.
    </dif>
    <refgrp tip="vid">
      <ref cel="embraz.0">embrazuro</ref>.
    </refgrp>
    <trd lng="en">crenellation</trd>
  </snc>
  <trd lng="ru">&c_z;&c_u;&c_b;&c_je;&c_c; &c_s;&c_t;&c_je;&c_n;&c_y;</trd>
</drv>

<drv mrk="krenel.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>ojn:
      <ekz>
        <tld/>i muron.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="brecx">bre&ccirc;eti</ref>,
      <ref cel="dent">denti</ref>,
      <ref cel="feston">festoni</ref>.
    </refgrp>
    <trd lng="en">crenellate</trd>
  </snc>
</drv>

<drv mrk="krenel.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Havanta <tld/>ojn:
      <ekz>
        <tld/>a muro.
      </ekz>
    </dif>
    <trd lng="en">crenellated</trd>
  </snc>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Havanta tran&ccirc;ojn similajn al <tld/>o:
      <ekz>
        <tld/>aj folioj.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>crenate</trd>,
      <trd>notched</trd>,
      <trd>scalloped</trd>,
      <trd>toothed</trd>
    </trdgrp>
  </snc>
</drv>
</art>
