<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>iran</rad>/o
</kap>

<drv mrk="iran.0o">
  <kap>
    <tld lit="I"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="iran.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <mlg>IR</mlg>
    <dif>
      &Scirc;tato en la sudokcidenta Azio kun &ccirc;efurbo Tehrano.
      Oficiala nomo: <em>Islama Respubliko de Irano</em>.
      &Gcirc;i limas kun <ref tip="vid"
      cel="afgan.0ujo.GEOG">Afganujo</ref>,
      <ref tip="vid" cel="armen.0ujo.GEOG">Armenujo</ref>,
      <ref tip="vid" cel="azerbajg.0o.GEOG">Azerbaj&gcirc;ano</ref>,
      <ref tip="vid" cel="irak.0o.GEOG">Irako</ref>,
      <ref tip="vid" cel="pakistan.0o.GEOG">Pakistano</ref>,
      <ref tip="vid" cel="turkmen.0ujo.GEOG">Turkmenujo</ref>,
      <ref tip="vid" cel="turk.T0ujo.GEOG">Turkujo</ref>
      kaj kun Araba Maro kaj <ref tip="vid"
      cel="pers.P0aGolfo.GEOG">Persa Golfo</ref>.
      &Gcirc;is 1935 &gcirc;i estis nomata <ref tip="sin"
      cel="pers.0ujo">Persujo</ref>.
    </dif>
    <trd lng="bg">&c_I;&c_r;&c_a;&c_n;</trd>
    <trd lng="de">Iran</trd>
    <trd lng="fa">&a_A;&f_y;&a_r;&a_A;&a_n;</trd>
    <trd lng="it">Iran</trd>
    <trdgrp lng="pl">
      <trd>Iran</trd>,
      <trd>Islamska Republika <ind>Iranu</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

