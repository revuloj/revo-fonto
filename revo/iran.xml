<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc><rad>Iran</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="iran.I0o">
  <kap>
    <ofc>8</ofc><tld/>o
     <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/aktoj/aktoj2/8oa.html"> 8a
Oficiala Aldono</url></vrk>, <lok>II.C.1</lok></fnt>
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>
  </kap>
  <snc mrk="iran.I0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <mlg>IR</mlg>
    <dif>
      &Scirc;tato en la sudokcidenta Azio kun &ccirc;efurbo Tehrano.
      Oficiala nomo: <em>Islama Respubliko de Irano</em>.
      &Gcirc;i limas kun <ref tip="vid"
      cel="afgan.A0ujo.GEOG">Afganujo</ref>,
      <ref tip="vid" cel="armen.A0ujo.GEOG">Armenujo</ref>,
      <ref tip="vid" cel="azerbajg.A0o.GEOG">Azerbaj&gcirc;ano</ref>,
      <ref tip="vid" cel="irak.I0o.GEOG">Irako</ref>,
      <ref tip="vid" cel="pakistan.P0o.GEOG">Pakistano</ref>,
      <ref tip="vid" cel="turkmen.T0ujo.GEOG">Turkmenujo</ref>,
      <ref tip="vid" cel="turk.T0ujo.GEOG">Turkujo</ref>
      kaj kun Araba Maro kaj <ref tip="vid"
      cel="pers.P0aGolfo.GEOG">Persa Golfo</ref>.
      &Gcirc;is 1935 &gcirc;i estis nomata <ref tip="sin"
      cel="pers.P0ujo">Persujo</ref>.
    </dif>
    <trd lng="be">&c_Ib;&c_r;&c_a;&c_n;</trd>
    <trd lng="bg">&c_I;&c_r;&c_a;&c_n;</trd>
    <trd lng="de">Iran</trd>
    <trd lng="en">Iran</trd>
    <trd lng="fa">&a_A;&f_y;&a_r;&a_A;&a_n;</trd>
    <trdgrp lng="fr">
      <trd>Iran</trd>,
      <trd>R&eacute;publique islamique d'Iran</trd>
    </trdgrp>
    <trd lng="id">Iran</trd>
    <trd lng="it">Iran</trd>
    <trdgrp lng="pl">
      <trd>Iran</trd>,
      <trd>Islamska Republika <ind>Iranu</ind></trd>
    </trdgrp>
    <trd lng="pt">Ir&atilde;</trd>
  </snc>
</drv>

<drv mrk="iran.0ano">
  <kap><tld lit="i"/>ano</kap>
  <snc>
    <dif>
      &Scirc;tatano de Irano
    </dif>
  </snc>
  <trdgrp lng="fr"> 
    <trd>Iranien<klr> (subst.)</klr></trd>
    <trd>Iranienne<klr> (subst.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="iran.0a">
  <kap><tld lit="i"/>a</kap>
  <snc>
    <dif>
    Rilata al la &scirc;tato Irano.
    </dif>
  </snc>
  <trdgrp lng="fr"> 
    <trd>iranien<klr> (adj.)</klr></trd>
    <trd>iranienne<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>

</art>

