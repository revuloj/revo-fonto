<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: iran.xml,v 1.26 2023/11/19 10:22:14 revo Exp $">
<kap>
  <ofc>8</ofc><rad>Iran</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="iran.I0o">
  <kap>
    <ofc>8</ofc><tld/>o
    <fnt><bib>RLN</bib></fnt>
  </kap>
  <mlg kod="ISO-3166">IR</mlg>
  <snc mrk="iran.I0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&Scirc;tato</ref>
      en la sudokcidenta <ref tip="malprt" cel="azi.A0o">Azio</ref>
      kun &ccirc;efurbo Tehrano;
      oficiala nomo: <em>Islama Respubliko de Irano</em>;
      limanta kun <ref tip="vid" cel="afgan.A0ujo.GEOG">Afganujo</ref>,
      <ref tip="vid" cel="armen.A0ujo.GEOG">Armenujo</ref>,
      <ref tip="vid" cel="azerbajg.A0o.GEOG">Azerbaj&gcirc;ano</ref>,
      <ref tip="vid" cel="irak.I0o.GEOG">Irako</ref>,
      <ref tip="vid" cel="pakistan.P0o.GEOG">Pakistano</ref>,
      <ref tip="vid" cel="turkmen.T0ujo.GEOG">Turkmenujo</ref>,
      <ref tip="vid" cel="turk.T0ujo.GEOG">Turkujo</ref>
      kaj kun Araba Maro kaj <ref tip="vid" cel="golf.Persa0o.GEOG">Persa Golfo</ref>.
      &Gcirc;is 1935 &gcirc;i estis nomata <ref tip="sin" cel="pers.P0ujo">Persujo</ref>:
      <ekz>
         el Sirio kaj <tld/>o, kie anka&ubreve; ekzistas tre antikvaj urboj, venis nenia reago
        <fnt>
          
          <vrk><url ref="&Monato;/1993/A930902.php">Noticoj</url></vrk>
          <bib>Monato</bib>
          <lok>1993/09, p. 4</lok>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve; statistikoj pereas pro a&ubreve;to-akcidentoj &ccirc;iujare en <tld/>o pli ol 15&nbsp;000 homoj
        <fnt>
          <aut>Hamizeh Shafiee</aut>
            <vrk><url ref="&Monato;/2001/007028.php">Traf(ik)a statistiko</url></vrk>
            <bib>Monato</bib>
            <lok>2001/01, p. 10</lok>
          </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_Ib;&c_r;&c_a;&c_n;</trd>
    <trd lng="bg">&c_I;&c_r;&c_a;&c_n;</trd>
    <trd lng="de">Iran</trd>
    <trd lng="en">Iran</trd>
    <trd lng="fa">&a_A;&f_y;&a_r;&a_A;&a_n;</trd>
    <trdgrp lng="fr">
      <trd>Iran</trd>,
      <trd>R&eacute;publique islamique d'Iran</trd>
    </trdgrp>
    <trd lng="id">Iran</trd>
    <trd lng="it">Iran</trd>
    <trd lng="nl">Iran</trd>
    <trdgrp lng="pl">
      <trd>Iran</trd>,
      <trd>Islamska Republika <ind>Iranu</ind></trd>
    </trdgrp>
    <trd lng="pt">Ir&atilde;</trd>
    <trd lng="uz">&c_E;&c_r;&c_o;&c_n;</trd>
    <trd lng="zh">&#x4F0A;&#x6717; <pr>Y&#x12B;l&#x1CE;ng</pr></trd>
</snc>
  <trd lng="bo">&#xF4F;&#xF0B;&#xF5F;&#xF72;&#xF42;&#xF0B;</trd>
  <trd lng="ja">&#x30A4;&#x30E9;&#x30F3;  <pr>&#x3044;&#x3089;&#x3093;</pr></trd>
  <trd lng="uk">&c_Ib;&c_r;&c_a;&#x43D;</trd>
</drv>

<drv mrk="iran.0a">
  <kap><tld lit="i"/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        la <tld lit="i"/>a prezidanta baloto de majo 1997
        <fnt>
          <aut>M. Reza Torabi/pg</aut>
            <vrk><url ref="&Monato;/2000/007017.php">Vundita sed ne mortinta</url></vrk>
            <bib>Monato</bib>
            <lok>2000/07, p. 7</lok>
          </fnt>;
      </ekz>
      <ekz>
        en aprilo estas liberigitaj 15 britaj mararmeanoj kaptitaj de <tld lit="i"/>aj 
        militistoj kaj enkarcerigitaj dum preska&ubreve; du semajnoj en <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2007/009225.php">Kiom valoras kaptiteco?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_r;&c_a;&c_n;&c_s;&c_k;&c_ib;</trd>
  <trd lng="de">iranisch</trd>
  <trdgrp lng="fr"> 
    <trd>iranien<klr> (adj.)</klr></trd>
    <trd>iranienne<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="iran.0ano">
  <kap><tld lit="i"/>ano</kap>
  <snc>
    <dif>
      &Scirc;tatano de <tld/>o:
      <ekz>
        <tld lit="i"/>ano e&ccirc; ne pensus, kiel ekzemple faras multe da libananoj,
        eduki siajn gefilojn en lingvo alia ol la persa
        <fnt>
          <bib>Monato</bib>
          <aut>Simone Zoppellaro</aut>
          <vrk><url ref="&Monato;/2014/011224.php">Lingvo 
          kiel sukero: notoj pri la nuntempa persa</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_r;&c_a;&c_n;&c_je;&c_c;</trd>
  <trd lng="de">Iraner</trd>
  <trdgrp lng="fr"> 
    <trd>Iranien<klr> (subst.)</klr></trd>
    <trd>Iranienne<klr> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4F0A;&#x6717;&#x4EBA; <pr>y&#x12B;l&#x1CE;ngr&#xE9;n</pr></trd>,
    <trd>&#x4F0A;&#x6717;&#x8A9E;&#x7CFB; <pr>y&#x12B;l&#x1CE;ngy&#x1D4;x&#xEC;</pr></trd>
  </trdgrp>
</drv>

</art>

