<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;elk</rad>/o
</kap>
<drv mrk="sxelk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Rimeno, ordinare elasta, kiun oni metas baltee sur &ccirc;iu 
&scirc;ultro por porti vestojn, kutime pantalonon.
    </dif>
  </snc>
  <snc>
    <dif>
      La&ubreve; plivastigo de signifo, afero kiu estas <tld/>oforma.
     <ekz>
     <tld lit="&Scirc;"/>oj de la pistolingo; <tld/>ozono por montogrimpado.
     </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_c;&c_ja;&c_zh;&c_k;&c_ib;</trd>,
    <trd>&c_sh;&c_l;&c_je;&c_j;&c_k;&c_ib;</trd>    
  </trdgrp>
  <trd lng="bg">&c_t;&c_i;&c_r;&c_a;&c_n;&c_t;&c_i;</trd>
  <trd lng="ca">el&agrave;stics</trd>
  <trd lng="de">Hosentr&auml;ger</trd>
  <trd lng="el">&tau;&jota;&rho;&alfa_ton;&ny;&tau;&epsilon;&sigma_fina;</trd>
  <trd lng="en">suspenders</trd>
  <trd lng="es">tirantes</trd>
  <trd lng="hu">nadr&aacute;gtart&oacute;</trd>
  <trd lng="fr">bretelle</trd>
  <trd lng="nl">bretel</trd>
  <trd lng="pt">suspens&oacute;rios</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_t;&c_ja;&c_zh;&c_k;&c_i;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_o;&c_ch;&c_i;</trd>
  </trdgrp>
  <trd lng="sv">h&auml;ngslen</trd>
  <trd lng="it">bretelle</trd>
</drv>
</art>
