<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lepr</rad>/o
</kap>
<drv mrk="lepr.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Infekta ha&ubreve;ta malsano, karakterizata per ulceroj kaj gangrenoj, kiuj
      iom post iom difektas kaj man&gcirc;etas la ha&ubreve;ton kaj karnon.
    </dif>
  </snc>
  <trd lng="en">leprosy</trd>
  <trd lng="es">lepra</trd>
  <trdgrp lng="nl">
    <trd>lepra</trd>,
    <trd>melaatsheid</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_k;&c_a;&c_z;&c_a; (&c_b;&c_o;&c_l;&c_je;&c_z;&c_n;&c_mol;)</trd>
</drv>
</art>
