<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc> <rad>&scirc;mir</rad>/i
</kap>
<drv mrk="sxmir.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxmir.0i.surmeti">
    <dif>
      Froti sur ion maldikan tavolon da grasa a&ubreve; alglui&gcirc;anta substanco:
      <ekz>
        <tld/>i iun per oleo<fnt>Z</fnt>,
      </ekz>
      <ekz>
        bulkon per butero, muron per mortero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi <tld/>os per veneno mian glavon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la falsa <tld/>o kovros la makulon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        dol&ccirc;e <tld/>i al iu la lipojn
        <klr>(f: multe promesi al iu.
        <refgrp tip="vid">
          <ref cel="miel.0o">mielo</ref>,
          <ref cel="or.0a">ora</ref>
        </refgrp>)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_m;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_b;&c_m;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_n;&c_a;&c_m;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">schmieren</trd>
    <trdgrp lng="fr">
      <trd>graisser</trd>,
      <trd>&eacute;taler<klr> (une substance grasse)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>beken</trd>,
      <trd>bed&ouml;rzs&ouml;l<klr> (ken&odblac;ccsel)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_n;&c_a;&c_m;&c_a;&c_z;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="sxmir.0i.lubriki">
    <dif>
      <ref tip="dif" cel="lubrik.0i">Lubriki</ref>:
      <ekz>
        kiu bone <tld/>as
        <klr>(la radojn. Vortludo, Vd <tld/>i 3)</klr>
        bone veturas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_m;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_m;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>schmieren</trd>,
      <trd>&ouml;len</trd>
    </trdgrp>
    <trd lng="hu">ken</trd>
    <trd lng="ru">&c_s;&c_m;&c_a;&c_z;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="sxmir.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="acxet.sub0i">Suba&ccirc;eti</ref>:
      <ekz>
        <tld/>i iun<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_d;&c_m;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="de">
      <trd>schmieren</trd>,
      <trd>bestechen</trd>
    </trdgrp>
    <trd lng="hu">megken<klr tip="ind"> (p&eacute;nzzel)</klr></trd>
    <trd lng="nl">omkopen</trd>
    <trd lng="ru">&c_p;&c_o;&c_d;&c_m;&c_a;&c_z;&c_a;&c_t;&c_mol; <klr>(&c_p;&c_je;&c_r;&c_je;&c_n;.)</klr></trd>
  </snc>
  <trd lng="en">smear</trd>
  <trd lng="nl">smeren</trd>
</drv>

<drv mrk="sxmir.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="&Scirc;"/>i &ccirc;e la &ccirc;irka&ubreve;o:
      <ekz>
        starigu al vi grandajn &scirc;tonojn kaj &ccirc;irka&ubreve;<tld/>u
        ilin per kalko
        <fnt><bib>MT</bib><lok>&Rea; 27:2</lok></fnt>.
      </ekz>
    </dif>
  </snc>
</drv>
</art>
