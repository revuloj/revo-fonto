<kap>
  <ofc>*</ofc> <rad>&scirc;mir</rad>/i
</kap>

<drv mrk="sxmir.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxmir.0i.surmeti">
    <dif>
      Froti sur ion maldikan tavolon da grasa a&ubreve;
      alglui&gcirc;anta substanco:
      <ekz>
        mi forlavis de vi vian sangon, kaj mi <tld/>is vin per oleo
        <fnt><bib>MT</bib>, <lok>&Jeh; 16:9</lok></fnt>;
      </ekz><ekz>
        mi detruos la muron, kiun vi <tld/>is per malta&ubreve;ga mortero
        <fnt><bib>MT</bib>, <lok>&Jeh; 13:14</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> prenis panon el la &scirc;ranko <klr>[&#8230;]</klr> kaj sur<tld/>is la
        marmeladon
        <fnt>
          <bib>ElektFab</bib>
          <lok>La kura&gcirc;a tajloreto</lok>
        </fnt>;
      </ekz><ekz>
        mi <tld/>os per veneno mian glavon
        <fnt><aut>&Scirc;ekspiro, tr. &Z;</aut>: <vrk>Hamleto</vrk></fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> <tld/>is siajn viza&gcirc;ojn per koto, kaj &scirc;utis cindron sur la
        harojn
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        la planko estis <tld/>vi&scirc;ata
        <fnt>
          <bib>Fab1</bib>
          <lok>Fluganta kofro</lok>
        </fnt>;
      </ekz><ekz>
        dol&ccirc;e <tld/>i al iu la lipojn
        <klr>(f: multe promesi al iu.
          <refgrp tip="vid">
            <ref cel="miel.0o">mielo</ref>,
            <ref cel="or.0a">ora</ref>
          </refgrp>)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <rim>
      Foje renkonti&gcirc;as inter&scirc;an&gcirc;o de
      objekto kaj komplemento:
      <ekz>
        li <tld/>as buteron sur pantran&ccirc;on
        <fnt><aut>&Scirc;timec</aut>:
          <vrk>Ombro sur interna pejza&gcirc;o</vrk></fnt>.
      </ekz>
    </rim>
    <trdgrp lng="be">
      <trd>&c_m;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_b;&c_m;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_n;&c_a;&c_m;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>einschmieren</trd>,
      <trd>ein&ouml;len</trd>,
      <trd>einkremen</trd>,
      <trd>salben</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>graisser</trd>,
      <trd>&eacute;taler<klr> (une substance grasse)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>beken</trd>,
      <trd>bed&ouml;rzs&ouml;l<klr> (ken&odblac;ccsel)</klr></trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>ungere</trd>,
      <trd>ingrassare<klr> (ungere)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>smarowa&cacute;</trd>,
      <trd>posmarowa&cacute;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>friccionar</trd>,
      <trd>ungir</trd>,
      <trd>besuntar</trd>,
      <trd>lambuzar</trd>
    </trdgrp>
    <trd lng="ru">&c_n;&c_a;&c_m;&c_a;&c_z;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="sxmir.0i.lubriki">
    <dif>
      <ref tip="dif" cel="lubrik.0i">Lubriki</ref>:
      <ekz>
        kiu bone <tld/>as
        <klr>(la radojn. Vortludo, Vd <tld/>i 3)</klr>
        bone veturas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_m;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_m;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>schmieren</trd>,
      <trd>einfetten</trd>,
      <trd>&ouml;len</trd>,
      <trd>einschmieren</trd>,
      <trd>ein&ouml;len</trd>,
      <trd>einkremen</trd>,
      <trd>salben</trd>
    </trdgrp>
    <trd lng="hu">ken</trd>
    <trd lng="it">lubrificare</trd>
    <trd lng="ru">&c_s;&c_m;&c_a;&c_z;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="sxmir.0i.skribacxi">
    <dif>
      Skriba&ccirc;i, <ref tip="sin" cel="sxmir.paper0i">paper&scirc;miri</ref>:
      <ekz>
        &ccirc;u ne estus bone, <klr>[&#8230;]</klr> se vi sidi&gcirc;us kaj kun<tld/>us ian po&scirc;libron, a&ubreve;
        almanakon <klr>[&#8230;]</klr>, kiel estas nun modo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>?
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>schmieren</trd>,
      <trd>herumschmieren</trd>,
      <trd>schmotzen<klr> [s&uuml;dd]</klr></trd>
    </trdgrp>
    <trd lng="be">&c_k;&c_r;&c_e;&c_m;&c_z;&c_a;&c_c;&c_mol;</trd>
  </snc>
  <snc mrk="sxmir.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="acxet.sub0i">Suba&ccirc;eti</ref>:
      <ekz>
        <tld/>ante la manojn de politikistoj
        <fnt>
          <bib>Monato</bib>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url ref="&Monato;/2005/008691.php">Familia nomo</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu produktanto <klr>[&#8230;]</klr> <tld/>is per kiom da mono kiujn burokratojn en
        Bruselo por ellabori tiajn preskribojn
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2005/008747.php">EU-fiasko</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_d;&c_m;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_d;&c_k;&c_u;&c_p;&c_ib;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>schmieren</trd>,
      <trd>bestechen</trd>,
      <trd>herumschmieren</trd>,
      <trd>schmotzen<klr> [s&uuml;dd]</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>acheter<klr tip="ind"> (soudoyer)</klr></trd>,
      <trd>corrompre<klr tip="ind"> (par de l'argent)</klr></trd>,
      <trd>payer<klr tip="ind"> (soudoyer)</klr></trd>,
      <trd>graisser la patte (&agrave;)</trd>,
      <trd>soudoyer</trd>,
      <trd>suborner<klr tip="ind"> (par de l'argent)</klr></trd>
    </trdgrp>
    <trd lng="hu">megken<klr tip="ind"> (p&eacute;nzzel)</klr></trd>
    <trdgrp lng="it">
      <trd>ungere<klr> (corrompere)</klr></trd>,
      <trd>corrompere</trd>
    </trdgrp>
    <trd lng="nl">omkopen</trd>
    <trdgrp lng="pl">
      <trd>smarowa&cacute;</trd>,
      <trd>posmarowa&cacute;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>subornar</trd>,
      <trd>molhar a m&atilde;o</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_o;&c_d;&c_m;&c_a;&c_z;&c_a;&c_t;&c_mol;
      <klr>(&c_p;&c_je;&c_r;&c_je;&c_n;.)</klr></trd>
  </snc>
  <trd lng="ca">untar</trd>
  <trd lng="en">smear</trd>
  <trd lng="es">untar</trd>
  <trdgrp lng="ja">
    <trd>&#22615;&#x308B;  <pr>&#x306C;&#x308B;</pr></trd>,
    <trd>&#22615;&#x5E03;&#12377;&#x308B; <pr>&#12392;&#12405;&#12377;&#12427;</pr></trd>,
    <trd>&#x30B0;&#x30EA;&#x30FC;&#x30B9;&#12434;&#22615;&#x308B;
    <pr>&#x30B0;&#x30EA;&#x30FC;&#x30B9;&#12434;&#x306C;&#x308B;</pr></trd>,
    <trd>&#x6CB9;&#12434;&#x5DEE;&#12377;  <pr>&#12354;&#12406;&#12425;&#12434;&#12373;&#12377;</pr></trd>,
    <trd>&#x8CB7;&#x53CE;&#12377;&#x308B;  <pr>&#12400;&#12356;&#12375;&#12421;&#12358;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">smeren</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_a;&c_z;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43D;&c_a;&#x43C;&c_a;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43C;&c_a;&c_s;&c_t;&c_i;&c_t;&c_i;</trd>,
    <trd>&#x43F;&#x43E;&#x43A;&c_r;&c_i;&c_v;&c_a;&c_t;&c_i; (&#x43C;&c_a;&c_z;&c_z;&#x44E;</trd>,
    <trd>&c_zh;&c_i;&c_r;&#x43E;&#x43C; &c_ib; &c_t;.&#x43F;.)</trd>
  </trdgrp>
</drv>

<drv mrk="sxmir.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        &scirc;i anta&ubreve;faris la <tld/>on de mia korpo por la entombigo
        <fnt>
          <bib>NT</bib>
          <lok>Marko 14:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        la falsa <tld/>o kovros la makulon
        <fnt>
          <bib>Hamlet</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_m;&c_a;&c_z;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="de">Schmierung</trd>
  <trdgrp lng="pl">
    <trd>przekupstwo</trd>,
    <trd>namaszczenie</trd>,
    <trd>&lstroke;apownictwo</trd>
  </trdgrp>
  <trd lng="uk">&c_jeu;&#x43B;&c_je;&#x43E;&#x43F;&#x43E;&#x43C;&c_a;&c_z;&c_a;&#x43D;&#x43D;&#x44F;</trd>
</drv>

<drv mrk="sxmir.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc>
    <dif>
      Io, per kio oni <tld/>as:
      <ekz>
        kelkaj pretigadis la <tld/>a&jcirc;ojn el aroma&jcirc;oj
        <fnt>
          <bib>MT</bib>
          <lok>I. Kroniko 9:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        Maria prenis funton da pura narda <tld/>a&jcirc;o tre multekosta, &scirc;miris la piedojn de
        Jesuo
        <fnt>
          <bib>NT</bib>
          <lok>Johano 12:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i frotis Elizon per suko de juglando, tiel ke &scirc;i fari&gcirc;is tute mallume bruna,
        &scirc;miris &scirc;ian belan viza&gcirc;on per malbonodora <tld/>a&jcirc;o kaj malordigis &scirc;iajn
        belajn harojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        kontra&ubreve; la kunglui&gcirc;ado de la palpebroj <klr>[&#8230;]</klr> froti sur la fermitaj
        palpebroj <tld/>a&jcirc;on kun blanka hidrarga precipitato
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        sunprotekta <tld/>a&jcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2005/008747.php">EU-fiasko</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_z;&c_mol;</trd>,
    <trd>&c_z;&c_m;&c_a;&c_z;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&#xF56;&#xFB1;&#xF74;&#xF42;&#xF0B;</trd>
  <trdgrp lng="de">
    <trd>Salbe</trd>,
    <trd>Schmiere</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#22615;&#x308A;&#x85AC;  <pr>&#x306C;&#x308A;&#12368;&#12377;&#x308A;</pr></trd>,
    <trd>&#22615;&#x308A;&#x6CB9;  <pr>&#x306C;&#x308A;&#12354;&#12406;&#12425;</pr></trd>,
    <trd>&#x30AF;&#x30EA;&#x30FC;&#x30E0;  <pr>&#x304F;&#x308A;&#x30FC;&#12416;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&lstroke;ap&oacute;wka</trd>,
    <trd>ma&zacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_a;&c_z;&#x44C;</trd>,
    <trd>&c_z;&#x43C;&c_a;&c_z;&#x43A;&c_a;</trd>,
    <trd>&#x43C;&c_a;&c_s;&c_t;&c_i;&#x43B;&#x43E;</trd>,
    <trd>&#x43C;&c_a;&c_s;&c_t;&c_i;&#x43A;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="sxmir.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="&Scirc;"/>i &ccirc;e la &ccirc;irka&ubreve;o:
      <ekz>
        &scirc;i prenis por li keston el kanoj kaj &ccirc;irka&ubreve;<tld/>is &gcirc;in per asfalto kaj pe&ccirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 2:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        starigu al vi grandajn &scirc;tonojn kaj &ccirc;irka&ubreve;<tld/>u
        ilin per kalko
        <fnt><bib>MT</bib>, <lok>&Rea; 27:2</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_m;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">einschmieren</trd>
  <trd lng="pl">zasmarowa&cacute;</trd>
</drv>

<drv mrk="sxmir.paper0i">
  <kap>paper<tld/>i</kap>
  <snc mrk="sxmir.paper0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Skribi multajn sed senvalorajn aferojn:
      <ekz>
        trovi&gcirc;os plumknaristo, paper<tld/>isto, kiu e&ccirc;
        en komedion vin enmetos
        <fnt><bib>Revizoro</bib>, <lok>akto 5a, sceno 8a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_c;&c_k;&c_a;&c_c;&c_mol; <ind>&c_p;&c_a;&c_p;&c_je;&c_r;&c_u;</ind></trd>
  <trd lng="br">paperaji&ntilde;</trd>
  <trdgrp lng="de">
    <trd>aufs Papier schmieren</trd>,
    <trd>hinschmieren</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire de la <ind>paperasse</ind></trd>,
    <trd>gratter le papier</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>firk&aacute;l</trd>,
    <trd>csak pazarolja a <ind>pap&iacute;rt</ind></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>scarabocchiare<klr> (fig.)</klr></trd>,
    <trd>scribacchiare<klr> (senza valore)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x66F8;&#x304D;&#x4E26;&#12409;&#x308B;
  <pr>&#x304B;&#x304D;&#x306A;&#12425;&#12409;&#x308B;</pr></trd>
  <trd lng="nl">bekladden</trd>
  <trdgrp lng="pl">
    <trd>smarowa&cacute;<klr> (pisa&cacute; g&lstroke;upoty)</klr></trd>,
    <trd>obsmarowa&cacute;<klr> kogo&sacute; (pisa&cacute; o kim&sacute; &zacute;le)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_a;&c_r;&c_a;&c_t;&c_mol;
    &c_b;&c_u;&c_m;&c_a;&c_g;&c_u;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i;/&#x43F;&c_je;&c_r;&c_je;&c_v;&#x43E;&c_d;&c_i;&c_t;&c_i; &#x43F;&c_a;&#x43F;&c_ib;&c_r;</trd>,
    <trd>&#x43F;&#x43E;&#x43F;&c_i;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="sxmir.0mono">
  <kap>
    <tld/>mono
  </kap>
  <snc>
    <dif>
      Mono por suba&ccirc;eti:
      <ekz>
        kie mankas spaco en tombejoj, nepras pagi <tld/>monon al la a&ubreve;toritatoj por
        ricevi enterigejon
        <fnt>
          <bib>Monato</bib>
          <aut>Princo Henriko Oguinye/pg</aut>
          <vrk><url ref="&Monato;/2006/008821.php">Forpasi, elspezi</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        suferas kamparanoj pro atakoj faritaj de bov&scirc;telistoj, dum &gcirc;endarmoj kaj
        policistoj postulas <tld/>monon de homoj kontrolataj
        <fnt>
          <bib>Monato</bib>
          <aut>Fidilalao Henriel</aut>
          <vrk><url ref="&Monato;/2012/010816.php">Restas nur espero</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_a;&c_b;&c_a;&c_r;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_k;&c_u;&c_p;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_m;&c_a;&c_z;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Schmiergeld</trd>
  <trd lng="ja">&#x8CC4;&#x8CC2;  <pr>&#x308F;&#12356;&#x308D;</pr></trd>
  <trd lng="pl">&lstroke;ap&oacute;wka</trd>
  <trd lng="uk">&c_h;&c_a;&c_b;&c_a;&c_r;</trd>
</drv>
</art>
