    <trd>&#x3075;&#x3056;&#x3051;&#x5C4B;  <pr>&#x3075;&#x3056;&#x3051;&#x3084;</pr></trd>
  </trdgrp>
  <trd lng="nl">stoeipoes</trd>
  <trdgrp lng="pl">
