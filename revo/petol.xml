  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_u;&c_s;&c_t;&c_u;&c_n;</trd>,
    <trd>&c_sh;&c_t;&c_u;&c_k;&c_a;&c_r;</trd>,
    <trd>&c_sh;&c_i;&c_b;&c_je;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
