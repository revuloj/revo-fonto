<drv mrk="petol.0emo">
  <kap><tld/>emo</kap>
  <snc mrk="petol.0emo.KOMUNE">
    <dif>
      Verva, ludema, &scirc;erca gajo:
      <ekz>
        estas insida fulmeto de &gcirc;ojo kaj ribelemo kaj <tld/>emo en la naturo &ccirc;i tie kaj sekve anka&ubreve;
        en la infanoj
        <fnt>
          <aut>E. Turner, trad. R. McGrath</aut>
          <vrk><url ref="https://eduinf.waw.pl/esp/lern/librejo/0144/0001.php">Sep Junaj
          A&ubreve;stralianoj</url></vrk>
          <lok>1994</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_r;&c_e;&c_mol;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_a;&c_r;&c_e;&c_z;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_u;&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">dov&aacute;divost</trd>
  <trdgrp lng="de">
    <trd>Verspieltheit</trd>,
    <trd>Albernheit</trd>,
    <trd>Ausgelasseneheit</trd>
  </trdgrp>
  <trd lng="en">playfulness</trd>
  <trdgrp lng="fr">
    <trd>badinerie<klr tip="amb"> (disposition d'esprit)</klr></trd>,
    <trd>espi&egrave;glerie<klr tip="amb"> (disposition
    d'esprit)</klr></trd>
  </trdgrp>
  <trd lng="pl">swawola</trd>
  <trd lng="ro">joc</trd>
  <trdgrp lng="sk">
    <trd>roztopa&scaron;</trd>,
    <trd>samopa&scaron;</trd>
  </trdgrp>
</drv>

<drv mrk="petol.0ulo">
