<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dis.xml,v 1.47 2023/11/04 14:29:03 revo Exp $">
<kap>
  <ofc>*</ofc><rad>dis</rad>-
  <fnt><bib>PV</bib></fnt>
</kap>
<subart>
<drv mrk="dis.0">
  <kap><tld/>-</kap>
  <snc mrk="dis.0.prefikso">
    <dif>
      Prefikso esprimanta malkuni&gcirc;on, dividon, fori&gcirc;on en
      diversajn direktojn:
      <ekz>
        <klr>[la tola&jcirc;on]</klr> oni <tld/>&scirc;iris <klr>[&#x2026;]</klr>, 
        <tld/>hakis, <tld/>premis
        kaj kuiris <klr>[&#x2026;]</klr>, fari&gcirc;is bela blanka delikata papero
        <fnt>
          <bib>Fab3</bib>
          <lok>Lino</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>&scirc;iris la leteron kaj <tld/>&jcirc;etis &gcirc;iajn pecetojn en &ccirc;iujn angulojn
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[niaj]</klr> membroj estas <tld/>&jcirc;etitaj en urboj, urbetoj kaj vila&gcirc;oj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni vidis olivan arbaron <klr>[&#x2026;]</klr> kaj &ccirc;ie dense <tld/>semitajn daktilarbojn
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        cigno <tld/>pelis per la fortaj batoj de siaj flugiloj la krepuskan nebulon
        <fnt>
          <bib>Fab3</bib>
          <lok>Cigna nesto</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi <tld/>kurigis viajn malamikojn
        <fnt><bib>MT</bib>, <lok>&Psa; 89:10</lok></fnt>;
      </ekz>
      <ekz>
        la bruletantaj karboj de la fajrujo <tld/>sendis nur malfortan lumon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni <tld/>sendos manifeston al &ccirc;iuj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> <tld/>batis la glacion per sia ligna &scirc;uo
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tiuj]</klr> ideoj <tld/>vasti&gcirc;os
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        la gardisto <tld/>etendis la manojn
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 11</lok></fnt>;
      </ekz>
      <ekz>
        <ref cel="don.dis0i"><tld/>doni</ref>, <ref cel="fal.dis0i"><tld/>fali</ref>, 
        <ref cel="pec.dis0igi"><tld/>pecigi</ref>, <ref cel="polv.dis0igxi">dispolvi&gcirc;i</ref>,
        <ref cel="port.dis0i"><tld/>porti</ref>, <ref cel="rab.dis0i"><tld/>rabi</ref>, 
        <ref cel="sxpruc.dis0i"><tld/>&scirc;pruci</ref>,<ref cel="trumpe.dis0i"><tld/>trumpeti</ref>, 
        <ref cel="versx.dis0i"><tld/>ver&scirc;i</ref>, <ref cel="romp.dis0i"><tld/>rompi</ref>.
      </ekz>
    </dif>
    <rim>
      &Ccirc;i tiu prefikso havas kelkfoje sencon tre proksiman al tiu
      de
      <em>mal-</em> a&ubreve; <em>sen-</em>:
      <ekz>
        <tld/>volvi <klr>(malvolvi)</klr>;
      </ekz>
      <ekz>
        <tld/>faldi <klr>(malfaldi)</klr>;
      </ekz>
      <ekz>
        <tld/>revigi <klr>(senrevigi)</klr>;
      </ekz>
      <ekz>
        <tld/>harmonia.
      </ekz>
    </rim>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;-</trd>
  <trdgrp lng="de">
    <trd>auseinander-</trd>,
    <trd>zer-</trd>
  </trdgrp>
  <trd lng="pl">roz-</trd>
  <trdgrp lng="pt">
    <trd>des-</trd>,
    <trd>dis-<klr> (Prefixo que indica desuni&atilde;o, separa&ccedil;&atilde;o,
    dispers&atilde;o)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;-</trd>
  </drv>
</subart>

<subart>
  <dif>
    Samsignifa vortero uzata memstare:
  </dif>
<drv mrk="dis.0a">
  <kap><tld/>a</kap>
  <snc mrk="dis.0a.KOMUNE">
    <dif>
      Maldensa, dismetita, dissemita:
      <ekz>
        <tld/>aj aretoj da arboj
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        en <tld/>aj <klr>(<ref cel="koher.ne0a">nekoheraj</ref>)</klr> pensoj 
        li eksidis apud la skribotablo
        <fnt>
          <bib>HsT</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi a&ubreve;dis <tld/>ajn <klr>(<ref cel="op.unu0a">unuopajn</ref>)</klr> frazojn diritajn 
        inter ili, dum ili forlasis la halon
        <fnt>
          <bib>KrM</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="distr.0i">distri</ref>,
      <ref cel="sporad.0a">sporada</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>auseinander</trd>,
      <trd>getrennt</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>dispers&eacute;</trd>,
      <trd>&eacute;parpill&eacute;</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>oddzielny</trd>,
      <trd>rozrzucony</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x62C6;&#x5206; <pr>ch&#x101;if&#x113;n</pr></trd>,
    <trd>&#x777D; <pr>ku&#xED;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="dis.0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="dis.0a.aroj">
      <fnt><bib>EKV</bib><lok>&para;83</lok></fnt>
      <dif>
        <klr>(p.p. du <ref cel="ar.0o.MAT">aroj</ref>)</klr>
        Tiaj, ke ili ne havas komunan elementon:
        <ekz>
          la paraj kaj neparaj nombroj konsistigas <tld/>ajn arojn;
        </ekz>
        <ekz>
          du aroj estas <tld/>aj, se kaj nur se ilia
          <ref tip="vid" cel="komun.0ajxo">komuna&jcirc;o</ref> estas
          malplena.
        </ekz>
      </dif>
      <trd lng="de">disjunkt<klr tip="ind"> (-e Menge)</klr></trd>
      <trd lng="en">disjoint<klr tip="ind"> (set)</klr></trd>
      <trd lng="fr"><klr tip="ind">(ensemble) </klr>disjoint</trd>
      <trd lng="pl"><klr tip="ind">(zbi&oacute;r)
      </klr>roz&lstroke;&aogonek;czny</trd>
      <trd lng="ru">&c_n;&c_je;&c_p;&c_je;&c_r;&c_je;&c_s;&c_je;&c_k;&c_a;&c_ju;&c_shch;&c_je;&c_je;&c_s;&c_ja;<klr tip="amb">
      (&c_m;&c_n;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
    </subsnc>
    <subsnc mrk="dis.0a.familio">
      <fnt><bib>PIV2</bib></fnt>
      <dif>
        <klr>(p.p. <ref cel="famili.0o.bildigo">familio</ref>)</klr>
        Tia, ke &ccirc;iuj ajn du termoj de &gcirc;i estas
        <ref cel="dis.0a.aroj"><tld/>aj</ref>:
        <ekz>
          la <ref cel="ar.kvocienta0o">kvocienta aro</ref> de
          ekvivalento-rilato ene de aro <frm><g>E</g></frm>
          konsistigas 
          <tld/>an familion de subaroj de <frm><g>E</g></frm>;
        </ekz>
        <ekz>
          por ke familio estu <tld/>a, ne sufi&ccirc;as, ke &gcirc;iaj
          termoj havu neniun komunan elementon.
        </ekz>
      </dif>
      <trd lng="de">disjunkt<klr tip="ind"> (-e Familie)</klr></trd>
      <trd lng="en">disjoint<klr tip="ind"> (family)</klr></trd>
      <trd lng="fr"><klr tip="amb">(famille) </klr>disjointe<klr tip="ind"> (math.)</klr></trd>
      <trd lng="pl">roz&lstroke;&aogonek;czna<klr tip="amb"> (rodzina
      zbior&oacute;w) </klr></trd>
      <trd lng="ru"><klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)
      </klr>&c_n;&c_je;&c_p;&c_je;&c_r;&c_je;&c_s;&c_je;&c_k;&c_a;&c_ju;&c_shch;&c_i;&c_h;&c_s;&c_ja;
      &c_m;&c_n;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;</trd>
    </subsnc>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_ja;&c_d;&c_n;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_d;&c_r;&c_o;&c_b;&c_l;&c_je;&c_n;&c_y;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_m;&c_je;&c_r;&c_k;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_d;&c_y;&c_s;&c_p;&c_e;&c_r;&c_g;&c_ib;&c_ja;&c_v;&c_a;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>disjunktivn&iacute;</trd>,
    <trd>disjunktn&iacute;</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>disperso</trd>,
    <trd>dispersa</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3061;&#x308A;&#x3062;&#x308A;&#x306E;</trd>,
    <trd>&#x3070;&#x3089;&#x3070;&#x3089;&#x306E;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_malmol;&c_je;&c_d;&c_i;&c_n;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_o;&c_b;&c_shch;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_r;&c_o;&c_z;&c_n;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_s;&c_je;&c_ja;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_d;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_r;&c_je;&c_d;&c_je;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
  <trd>riedky</trd>,
  <trd>rozpt&yacute;len&yacute;</trd>
</trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&#x43E;&c_z;&#x2019;&c_jeu;&c_d;&#x43D;&c_a;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_d;&c_ib;&#x43B;&c_je;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_ib;&c_d;&#x43E;&#x43A;&c_r;&c_je;&#x43C;&#x43B;&c_je;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="dis.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="dis.0eco.KOMUNE">
    <dif>
      Malkuneco, aparteco, malproksimeco, <ref cel="difere.0o">diferenco</ref>, 
      malamikeco:
      <ekz>
        la intergenta <tld/>eco
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Homaranismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>eco de mia propra familio
        <fnt>
          <aut>Anna L&ouml;wenstein</aut>
          <vrk>La &scirc;tona urbo</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="diaspor.0o">diasporo</ref>,
      <ref cel="distan.0o">distanco</ref>,
      <ref cel="kverel.0o.dauxra">kverelo</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Trennung</trd>,
      <trd>Zerstreutheit</trd>,
      <trd>Teilung</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x533A;&#x9694; <pr>q&#x16B;g&#xE9;</pr></trd>,
    <trd>&#x56DB;&#x6563; <pr>s&#xEC;s&#xE0;n</pr></trd>,
    <trd>&#x6563;&#x5FC3; <pr>s&#xE0;nx&#x12B;n</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_ja;&c_d;&c_n;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_d;&c_r;&c_o;&c_b;&c_l;&c_je;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>rozptyl</trd>,
    <trd>rozpt&yacute;lenost</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>dispersion</trd>,
    <trd>division<klr tip="ind"> (dispersion)</klr></trd>,
    <trd>&eacute;parpillement</trd>
  </trdgrp>
  <trd lng="he">&pe;&yod;&zayin;&vav;&resh;</trd>
  <trdgrp lng="ja">
    <trd>&#x6563;&#x5728;  <pr>&#x3055;&#x3093;&#x3056;&#x3044;</pr></trd>,
    <trd>&#x56DB;&#x6563;&#x3057;&#x305F;&#x72B6;&#x614B;  <pr>&#x3057;&#x3055;&#x3093;&#x3057;&#x305F;&#x3058;&#x3087;&#x3046;&#x305F;&#x3044;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>oddzielno&sacute;&cacute;</trd>,
    <trd>roz&lstroke;&aogonek;czno&sacute;&cacute;</trd>,
    <trd>rozbie&zdot;no&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_malmol;&c_je;&c_d;&c_i;&c_n;&c_jo;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_r;&c_o;&c_z;&c_n;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_o;&c_b;&c_shch;&c_jo;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">rozptyl</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&#x43E;&c_z;&#x2019;&c_jeu;&c_d;&#x43D;&c_a;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&c_v;&c_ib;&c_d;&#x43E;&#x43A;&c_r;&c_je;&#x43C;&#x43B;&c_je;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="dis.0igi">
  <kap><ofc>*</ofc><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="dis.0igi.KOMUNE">
    <dif>
      <ref tip="sin" cel="apart.0igi">Apartigi</ref>, <ref tip="sin" cel="divid.0i.partigi">dividi</ref>, 
      mal<ref tip="ant" cel="kun.0igi">kunigi</ref>:
      <ekz>
        kelkaj korteganoj <tld/>igas la batalantojn
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igitaj unu de alia per rivereto
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igitaj unu de la alia per la loko kaj tempo
        <fnt>
          <bib>Fab3</bib>
          <lok>Dorna vojo de la honoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        radio de fulmo <tld/>igis la aeron
        <fnt>
          <bib>Fab3</bib>
          <lok>Cigna nesto</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi <tld/>igis la interligon kun via sklavo
        <fnt><bib>MT</bib>, <lok>&Psa; 89:39</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>igitaj kiel &scirc;afoj ne havantaj pa&scirc;tiston
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 9:36</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili malfortigis la unuecon, <tld/>igis la socion
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &Hcirc;aldeano <tld/>igis la veston sur la brusto kaj eltiris oran medalon
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        per rapidaj pa&scirc;oj ili trairis la spacon <klr>[&#x2026;]</klr>, kiu <tld/>igis ilin de
        la parka pordo
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        longe <tld/>igita kaj nun denove kunigita familio
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiu doloro, kiu samtempe <tld/>igis kaj kunligis ilin
        <fnt><aut>Stellan Engholm</aut>: <vrk>Al Torento</vrk>, <lok>&ccirc;apitro
          IV, 1930</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="met">dismeti</ref>,
      <ref cel="jxet">dis&jcirc;eti</ref>,
      <ref cel="sem">dissemi</ref>,
      <ref cel="trancx.0i.KOMUNE">tran&ccirc;i</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_ja;&c_d;&c_n;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_l;&c_u;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_d;&c_z;&c_ja;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_b;&c_n;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_y;&c_s;&c_p;&c_e;&c_r;&c_g;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>odd&ecaron;lit</trd>,
    <trd>odlou&ccaron;it</trd>,
    <trd>rozd&ecaron;lit</trd>,
    <trd>rozlou&ccaron;it</trd>,
    <trd>rozpojit</trd>,
    <trd>rozpt&yacute;lit</trd>,
    <trd>rozpt&yacute;lit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>trennen</trd>,
    <trd>zerteilen</trd>,
    <trd>zerstreuen</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>separar</trd>,
    <trd>apartar</trd>,
    <trd>desunir</trd>,
    <trd>dividir</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>disperser</trd>,
    <trd>s&eacute;parer</trd>,
    <trd>diviser<klr tip="ind"> (s&eacute;parer)</klr></trd>,
    <trd>&eacute;parpiller</trd>
  </trdgrp>
  <trd lng="he">&lamed;&he;&pe;&resh;&yod;&dalet;</trd>
  <trdgrp lng="ja">
