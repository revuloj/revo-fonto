    <trd>&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_b;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_d;&c_y;&c_s;&c_p;&c_e;&c_r;&c_g;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de">sich <ind>trennen</ind></trd>
  <trdgrp lng="es">
