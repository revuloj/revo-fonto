<drv mrk="dis.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="dis.0igxi.KOMUNE">
    <dif>
      Iri al diversaj direktoj, mal<ref tip="ant" cel="kun.0igxi">kuni&gcirc;i</ref>:
      <ekz>
        ni <tld/>i&gcirc;is kaj iris en diversajn flankojn: mi iris dekstren, kaj li iris
        maldekstren
        <fnt>
          <bib>F</bib>
          <lok>&FE; 28</lok>
        </fnt>;
      </ekz>
      <ekz>
        la popolo <tld/>i&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 5:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vento malforti&gcirc;is, la nigraj nuboj <tld/>i&gcirc;is kaj oni vidis sur la &ccirc;ielo la
        sangan sunon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kurtenoj <klr>[&#x2026;]</klr> <tld/>i&gcirc;is kaj la bela infano ekridetis al la faraono
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        post alkutimi&gcirc;o doloras <tld/>i&gcirc;o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_ja;&c_d;&c_n;&c_o;&c_w;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
