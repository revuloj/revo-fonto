    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="dif" cel="krur.0umo.TEKS">Krurumo</ref>:
      <ekz>
        &scirc;i formetis siajn &scirc;uojn kaj &scirc;trumpetojn kaj 
        suprenrulis siajn pantalon<tld/>ojn &gcirc;isgenue
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Hosenbein</trd>
  </snc>
  <trd lng="cs">nohavice</trd>
  <trd lng="fr">jambe de pantalon</trd>
  <trd lng="pt">perna de cal&ccedil;a</trd>
  <trd lng="sk">nohavica</trd>
</drv>

<drv mrk="tub.prov0o">
  <kap>prov<tld/>o</kap>
  <snc mrk="tub.prov0o.KEM">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">KEM</uzo>
    <dif>
      <tld lit="T"/>forma vazeto, ordinare el vitro, en kiu
      oni provas reakciojn &ccirc;e flua&jcirc;oj a&ubreve; gasoj:
      <ekz>
        prov<tld/>etoj kaj analizoj ne lasas sin influi de komedio
        <fnt><bib>UrdHadda</bib>, <lok>p. 28a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Testr&ouml;hrchen</trd>,
      <trd>Reagenzglas</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="ca">
    <trd><ind>tub</ind> d'assaig</trd>,
    <trd>proveta</trd>
  </trdgrp>
  <trd lng="cs">zkumavka</trd>
  <trdgrp lng="fr">
    <trd>&eacute;prouvette</trd>,
    <trd>tube &agrave; essai</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&eacute;mcs&odblac;</trd>,
    <trd>lombik</trd>
  </trdgrp>
  <trd lng="nl">proefbuis</trd>
  <trd lng="pl">prob&oacute;wka</trd>
  <trd lng="pt">tubo de ensaio</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_b;&c_i;&c_r;&c_k;&c_a;</trd>
  <trd lng="sk">sk&uacute;mavka</trd>
</drv>

<drv mrk="tub.vaku0o">
  <kap>vaku<tld/>o</kap>
  <snc mrk="tub.vaku0o.RAD">
    <uzo tip="fak">RAD</uzo>
    <dif>
      Elektronika aran&gcirc;a&jcirc;o (ne plu uzata), vitra a&ubreve; metala vazo el
      kiu &ccirc;iuj gasoj estas forigitaj, kaj kies
      <ref tip="prt" cel="elektr3.0o">elektrodoj</ref> regas fluon
      de elektronoj:
      <ekz>
        la vicoj de vaku<tld/>oj ekglimis kaj la &scirc;altita televidilo
        malrapide varmi&gcirc;is
        <fnt>
          <aut>Pejno Simono</aut>:
          <vrk><url ref="http://ourworld.compuserve.com/homepages/profcon/e_konjak.htm">&Ccirc;u
          vi volas konjakon?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Vakuumr&ouml;hre</trd>
    <trd lng="en">vacuum tube</trd>
    <trd lng="fr"><ind>tube</ind> &agrave; vide</trd>
    <trdgrp lng="ru">
      <trd>&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_n;&c_n;&c_a;&c_ja; &c_l;&c_a;&c_m;&c_p;&c_a;</trd>,
      <trd>&c_r;&c_a;&c_d;&c_i;&c_o;&c_l;&c_a;&c_m;&c_p;&c_a;</trd>
    </trdgrp>
  </snc>
</drv>

