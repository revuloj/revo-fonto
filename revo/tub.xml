<drv mrk="tub.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tub.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Longa kava cilindroforma diversmateria ilo servanta por plej
      diversaj uzoj:ca
      <ekz>
        plumba, ka&ubreve;&ccirc;uka, vitra <tld/>o;
      </ekz>
      <ekz>
        <tld/>o de pipo, de trumpeto, de pafilo, de kanono;
      </ekz>
      <ekz>
        <ind>akvo<tld/>o</ind>;
        <trd lng="fr">tuyau d'arrosage</trd>
        <trd lng="hu">v&iacute;zcs&odblac;</trd>
        <trd lng="nl">afvoerpijp</trd>
        <trdgrp lng="pl">
          <trd>rura wodna</trd>,
          <trd>w&aogonek;&zdot; wodny</trd>,
          <trd>przew&oacute;d wodny</trd>
        </trdgrp>
        <trdgrp lng="pt">
          <trd>tubo</trd>
          <trd>cano</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind>gas<tld/>o</ind>;
        <trd lng="hu">g&aacute;zcs&odblac;</trd>
        <trdgrp lng="pl">
          <trd>rura gazowa</trd>,
          <trd>gazrurka</trd>,
          <trd>przew&oacute;d gazowy</trd>
        </trdgrp>
        <trd lng="nl">gasbuis</trd>
      </ekz>
      <ekz>
        <tld/>eto, en kiun oni metas cigaron, kiam oni &gcirc;in fumas, estas
        cigaringo
        <fnt>
          <bib>F</bib>
          <lok>&FE; 40</lok>
        </fnt>;
      </ekz>
      <ekz>
        vidu la hipopotamon <klr>[&#x2026;]</klr> &gcirc;iaj ostoj estas kiel kupraj
        <tld/>oj,
        &gcirc;iaj ostoj estas kiel feraj stangoj
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 40:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi vidas kandelabron <klr>[&#x2026;]</klr>, sep lucernoj estas sur &gcirc;i,
        kaj po sep
        <tld/>etoj trovi&gcirc;as sur la lucernoj
        <fnt>
          <bib>MT</bib>
          <lok>Ze&hcirc;arja 4:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malgranda knabo ekrigardis al la tekru&ccirc;o, la kovrilo levi&gcirc;is
        &ccirc;iam pli kaj
        pli, kaj la sambukaj floroj eliris fre&scirc;e kaj blanke, elkreskigis
        grandajn
        longajn bran&ccirc;ojn, e&ccirc; el la <tld/>eto ili etendi&gcirc;is &ccirc;iuflanken
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aperturo de pafilo
        <fnt>
          <bib>Fab4</bib>
          <lok>Malbona princo (Legendo)</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni enmetis la korpon en &scirc;tonan <tld/>on, tra kiu dum kelke da
        tagoj pasis
        varmega aero, kaj sekiginte oni transdonis al la enbalzamigistoj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        el la kaldrono elstaris vertikala <tld/>o, kovrita per peza
        &scirc;tono
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        li alsaltis al la kontra&ubreve;a muro kaj almetis la bu&scirc;on al la
        aperturo de <tld/>o,
        kondukanta al la supraj eta&gcirc;oj la vo&ccirc;on, parolantan malsupre
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        fajrestinga <tld/>o
        <klr>(vd <ref tip="sub" cel="hos.0o">hoso</ref>)</klr>
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i vidis en oksigena kesto la &scirc;rumpan violan bebon kun multaj
        <tld/>oj sur la
        korpo
        <fnt>
          <bib>Monato</bib>
          <aut>Ziyu</aut>
          <vrk><url ref="https://www.monato.be/2005/008556.php">Per
          patrina amo</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_t;&c_r;&c_u;&c_b;&c_a;</trd>,
      <trd>&c_t;&c_r;&c_u;&c_b;&c_k;&c_a;</trd>,
      <trd>&c_sh;&c_l;&c_a;&c_n;&c_g;</trd>,
      <trd>&c_t;&c_ju;&c_b;&c_i;&c_k;</trd>
    </trdgrp>
    </snc>
  <snc mrk="tub.0o.RAD">
    <uzo tip="fak">RAD</uzo>
    <dif>
      <ref cel="tub.vaku0o">Vaku<tld/>o</ref>:
      <ekz>
        ilia ekipa&jcirc;o konsistas el du ikso-radiaj <tld/>oj: temas pri
        kolimatiloj kaj
        detektiloj de amba&ubreve; flankoj de &ccirc;iu (plejofte pri tio nekonscia)
        pasa&gcirc;ero
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="https://www.monato.be/2013/011032.php">Skanado &gcirc;ena kaj kancerogena</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">vacuum tube</trd>
    <trd lng="ru">&c_r;&c_a;&c_d;&c_i;&c_o;&c_l;&c_a;&c_m;&c_p;&c_a;</trd>
  </snc>
  <trd lng="af">buis</trd>
  <trd lng="am">&#x1267;&#x1295;&#x1267;</trd>
  <trd lng="ar">&#x623;&#x646;&#x628;&#x648;&#x628;</trd>
  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_u;&c_b;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_u;&c_b;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_l;&c_a;&c_n;&c_g;</trd>,
    <trd>&c_c;&c_ju;&c_b;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_t;&c_r;&c_malmol;&c_b;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_k;&c_u;&c_ch;<ind> (flekstubo)</ind></trd>
  </trdgrp>
  <trd lng="bn">&#x99F;&#x9BF;&#x989;&#x9AC;</trd>
  <trdgrp lng="ca">
    <trd>tub</trd>,
    <trd>canonada</trd>,
    <trd>can&oacute;</trd>
  </trdgrp>
  <trd lng="co">tubu</trd>
  <trdgrp lng="cs">
    <trd>hlave&ncaron;</trd>,
    <trd>roura</trd>,
    <trd>trouba</trd>,
    <trd>trubice</trd>,
    <trd>trubka</trd>
  </trdgrp>
  <trd lng="da">r&#xF8;r</trd>
  <trdgrp lng="de">
    <trd>Rohr</trd>,
    <trd>R&ouml;hre</trd>,
    <trd>Leitung</trd>,
    <trd>Schlauch<ind> (flekstubo)</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>tube</trd>
    <trd>hose<ind> (flekstubo)</ind></trd>
  </trdgrp>
  <trd lng="et">toru</trd>
  <trd lng="eu">hodi</trd>
  <trdgrp lng="fr">
    <trd>tube</trd>,
    <trd>tuyau</trd>
  </trdgrp>
  <trd lng="fy">tube</trd>
  <trd lng="gl">tubo</trd>
  <trd lng="gu">&#xA9F;&#xACD;&#xAAF;&#xAC1;&#xAAC;</trd>
  <trd lng="hi">&#x91F;&#x94D;&#x92F;&#x942;&#x92C;</trd>
