<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tub</rad>/o
</kap>
<drv mrk="tub.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Longa kava cilindroforma diversmateria ilo servanta por plej
      diversaj uzoj:
      <ekz>
        plumba, ka&ubreve;&ccirc;uka, vitra <tld/>o;
      </ekz>
      <ekz>
        <tld/>o de pipo, de trumpeto, de pafilo, de kanono;
      </ekz>
      <ekz>
        akvo<tld/>o;
      </ekz>
      <ekz>
        gas<tld/>o;
      </ekz>
      <ekz>
        kamen<tld/>o;
      </ekz>
      <ekz>
        <tld/>eto, en kiun oni metas cigaron kiam oni &gcirc;in fumas, estas
        cigaringo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Rohr</trd>
  <trd lng="en">tube</trd>
  <trd lng="fr">tube</trd>
  <trd lng="nl">buis</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_u;&c_b;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_u;&c_b;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_l;&c_a;&c_n;&c_g;</trd>,
    <trd>&c_t;&c_ju;&c_b;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
