<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: levkoj.xml,v 1.27 2024/01/15 16:14:09 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>levkoj</rad>/o
</kap>
<drv mrk="levkoj.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="levkoj.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="brasik1.0oj">brasikacoj</ref>
      (<trd lng="la">Matthiola</trd>), 
      <ref tip="lst" cel="herb.0o.BOT" lst="voko:brasikoparencoj">herbo</ref>
      a&ubreve; duonarbusto kun bonodoraj, blankaj, 
      violet- a&ubreve; purpuretkoloraj floroj:
      <ekz>
        plej &ccirc;armaj <tld/>oj kaj diantoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur <tld/>o rampis ra&ubreve;po
        <fnt>
          <bib>Fab4</bib>
          <lok>Sterkskarabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        roza <tld/>o ornamis la malobeulojn
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        graciaj, delikataj <tld/>oj, blankaj kiel la unua ne&gcirc;o
        <fnt>
        <aut>Ivan G. &Scirc;irjaev</aut>,
        <vrk>Sen titolo</vrk>,
        <lok>&ccirc;apitro 1a</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/8/81/Matthiola_incana10.jpg/360px-Matthiola_incana10.jpg" prm="CC-BY-SA-3.0"><tld/>o (Matthiola incana)
        <fnt>
          <aut>Kenpei, propra foto</aut>
          <lok><url ref="&WCW;/File:Matthiola_incana10.jpg">&WCW;/File:Matthiola_incana10.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_w;&c_k;&c_o;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="ca">violer</trd>
  <trdgrp lng="cs">
    <trd>fiala letn&iacute;</trd>,
    <trd>levkoje</trd>
  </trdgrp>
  <trd lng="de">Levkoje</trd>
  <trdgrp lng="en">
    <trd>wallflower</trd>,
    <trd>gilliflower</trd>,
    <trd>stock</trd>
  </trdgrp>
  <trd lng="fa">&f_ch;&a_l;&f_y;&f_p;&a_A;</trd>
  <trd lng="fr">girofl&eacute;e des jardins</trd>
  <trd lng="hu">viola</trd>
  <trdgrp lng="ja">
    <trd>&#x30A2;&#x30E9;&#x30BB;&#x30A4;&#x30C8;&#x30A6;</trd>,
    <trd>&#x30B9;&#x30C8;&#x30C3;&#x30AF;</trd>
  </trdgrp>
  <trd lng="nl">violier</trd>
  <trd lng="pl">lewkonia</trd>
  <trdgrp lng="pt">
    <trd>goiveiro</trd>,
    <trd>goivo</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_je;&c_v;&c_k;&c_o;&c_j;</trd>
  <trd lng="sk">fiala no&ccaron;n&aacute;</trd>
  <trd lng="uk">&#x43B;&#x435;&#x432;&#x43A;&#x43E;&#x439;</trd>
  <trdgrp lng="zh">
    <trd>&#x7D2B;&#x7F57;&#x5170; <pr>z&#x1D0;lu&#xF3;l&#xE1;n</pr></trd>,
    <trd>&#x7D2B;&#x7F85;&#x862D; <pr>z&#x1D0;lu&#xF3;l&#xE1;n</pr></trd>
  </trdgrp>
</drv>
</art>
