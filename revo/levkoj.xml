<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>levkoj</rad>/o
</kap>
<drv mrk="levkoj.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="levkoj.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="brasik1.0oj">brasikacoj</ref>
      (<trd lng="la">Matthiola</trd>), kun bonodoraj floroj:
      <ekz>
        graciaj, delikataj <tld/>oj, blankaj kiel la unua ne&gcirc;o
        <fnt>
        <aut>Ivan G. &Scirc;irjaev</aut>,
        <vrk>Sen titolo</vrk>,
        <lok>&ccirc;apitro 1a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <adm>
    Kontrolinda.  La&ubreve; iuj fontoj la germana &leftquot;Levkoje&rightquot; signas
    apartan specion, science &leftquot;Matthiola incana&rightquot;.
    <aut>&jcirc;v</aut>
  </adm>
  <trd lng="be">&c_l;&c_ja;&c_w;&c_k;&c_o;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="ca">violer</trd>
  <trd lng="de">Levkoje</trd>
  <trdgrp lng="en">
    <trd>wallflower</trd>,
    <trd>gilliflower</trd>,
    <trd>stock</trd>
  </trdgrp>
  <trd lng="fa">&f_ch;&a_l;&f_y;&f_p;&a_A;</trd>
  <trd lng="fr">girofl&eacute;e des jardins</trd>
  <trd lng="hu">viola</trd>
  <trd lng="nl">violier</trd>
  <trd lng="pl">lewkonia</trd>
  <trdgrp lng="pt">
    <trd>goiveiro</trd>,
    <trd>goivo</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_je;&c_v;&c_k;&c_o;&c_j;</trd>
</drv>
</art>
