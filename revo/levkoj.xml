<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>levkoj</rad>/o
</kap>
<drv mrk="levkoj.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="brasik1.0oj">brasikacoj</ref>
      (<trd lng="la">Matthiola</trd>), kun bonodoraj floroj.
    </dif>
  </snc>
  <trd lng="de">Levkoje</trd>
  <trdgrp lng="en">
    <trd>gilliflower</trd>,
    <trd>stock</trd>
  </trdgrp>
  <trd lng="nl">violier</trd>
  <trd lng="ru">&c_l;&c_je;&c_v;&c_k;&c_o;&c_j;</trd>
</drv>
</art>
