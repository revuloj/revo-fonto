<art mrk="$Id$">
<kap>
<rad>rut</rad>/o
</kap>
<drv mrk="rut.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="rut.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Ruta</trd>),
      herbo kun forta odoro, uzata en medicino.
    </dif>
  </snc>
  <bld lok="../bld/rut.jpg">
    <ind>vin<tld/>o</ind>
    <trd lng="la">Ruta graveolens</trd>
  </bld>
  <trd lng="de">Raute</trd>
</drv>
<drv mrk="rut.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Rutaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Rautengew&auml;chse</trd>
</drv>
</art>
