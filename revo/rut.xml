<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>rut</rad>/o
</kap>
<drv mrk="rut.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="rutac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Ruta</trd>),
      herbo kun forta odoro, uzata en medicino.
    </dif>
  </snc>
  <bld lok="../bld/rut.jpg">
    <ind>vin<tld/>o</ind>
    <trd lng="la">Ruta graveolens</trd>
  </bld>
  <trd lng="de">Raute</trd>
  <trd lng="en">rue</trd>
  <trd lng="hu">ruta</trd>
  <trd lng="nl">wijnruit</trd>
  <trd lng="ru">&c_r;&c_u;&c_t;&c_a;</trd>
</drv>
</art>
