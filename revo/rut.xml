<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>rut</rad>/o
</kap>
<drv mrk="rut.0o">
  <kap><tld/>o</kap>
  <snc mrk="rut.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Iom ligna herbo kun forta odoro, kurace uzata, kreskanta de
      Mediteraneo &gcirc;is Okcidentazio, <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt"
      cel="rutac.0oj"><tld/>acoj</ref> (<trd lng="la">Ruta</trd>):
      <ekz>
        la edzino de &Scirc;afi metis ungventon sur la &scirc;veldikan vangon kaj
        fumigis <tld/>on
        <fnt>
        <aut>Mahbube Mirkadiri</aut>,
        <vrk>Donaco el voja&gcirc;o</vrk>,
        <bib>LOdE</bib>,
        <lok>2003:3 (101)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/rut.jpg">
    <ind>vin<tld/>o</ind>
    <trd lng="fr">rue f&eacute;tide</trd>
    <trd lng="la">Ruta graveolens</trd>
  </bld>
  <trd lng="be">&c_r;&c_u;&c_t;&c_a;</trd>
  <trd lng="ca">ruda</trd>
  <trd lng="de">Raute</trd>
  <trd lng="en">rue</trd>
  <trd lng="fr">rue<klr tip="amb"> (plante)</klr></trd>
  <trd lng="hu">ruta</trd>
  <trd lng="nl">wijnruit</trd>
  <trd lng="pt">arruda</trd>
  <trd lng="ru">&c_r;&c_u;&c_t;&c_a;</trd>
</drv>
</art>
