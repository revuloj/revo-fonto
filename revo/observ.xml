    <trd>pozorovate&lcaron;&ncaron;a</trd>,
    <trd>rozh&lcaron;ad&ncaron;a</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_p;&c_o;&c_s;&c_t;&c_je;&c_r;&c_je;&c_zh;&c_n;&c_i;&c_j; &c_p;&c_u;&c_n;&c_k;&c_t;</trd>
  <trdgrp lng="zh">
