  </trdgrp>
</drv>

<drv mrk="observ.0anto">
  <kap><tld/>anto</kap>
  <snc mrk="observ.0anto.KOMUNE">
    <dif>
      Atenta, zorga rigardanto:
      <ekz>
        <tld/>anto facile rimarkos, ke la ka&ubreve;zo de la balbutado ku&scirc;as <klr>[&#x2026;]</klr> en
        la nesufi&ccirc;a ekzerciteco de la diritaj personoj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista en Boulogne sur
           Mer en la 5a de a&ubreve;gusto 1905</vrk>
        </fnt>;
      </ekz>
      <ekz>
        oni ne bezonis esti tre sagaca <tld/>anto, por penetri &gcirc;is fundo la dol&ccirc;an
        sekreton
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekunua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>anto povus rimarki, ke la viza&gcirc;o de la prokuratoro
        <klr>[&#x2026;]</klr> esprimas malpaciencon
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 25a, p. 260a</lok></fnt>;
      </ekz>
      <ekz>
        meteologiaj verkoj kaj raportoj de <tld/>antoj
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="observ.0anto.komisiito">
    <dif>
      <ref tip="super" cel="komisi.0ito">Komisiito</ref> <tld/>i agadon sen aktive partopreni:
      <ekz>
        interparolante kun la <tld/>anto de laboro
        <klr>(vd <ref cel="labor.0estro">laborestro</ref>)</klr>
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        iru, starigu <tld/>anton, kion li vidos, li diru
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 21:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">POL</uzo>
        la Konferencon &ccirc;eestis kiel <tld/>antoj anka&ubreve; la oficialaj reprezentantoj de la
        dana Ministerio por Instruado kaj Klerigo
        <fnt>
          <bib>EeP</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>antoj de UEA partoprenis en la Seminario de UN pri la Homaj Rajtoj en
        Multnaciaj Socioj, okazinta en <nom>Beograd</nom>
        <fnt>
          <bib>EeP</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        registaroj de 16 &scirc;tatoj sendis siajn oficialajn <tld/>antojn
        <fnt>
          <bib>EeP</bib>
          <lok>Kvina Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="observ.0anto.obeanto">
    <dif>
      Iu, kiu <tld/>as<sncref ref="observ.0i.obei"/>:
      <ekz>
        la <tld/>antoj de la le&gcirc;oj indignas kontra&ubreve; li
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 28:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        la a&ubreve;skultantoj de la vortoj de la profeta&jcirc;o kaj la <tld/>antoj de la
        skribita&jcirc;oj en &gcirc;i
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 1:3</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_z;&c_ib;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_n;&c_a;&c_g;&c_l;&c_ja;&c_d;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
