<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>observ</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="observ.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="observ.0i.rigardi">
    <dif>
      <ref tip="super" cel="rigard.0i">Rigardi</ref> ion ade,
      kun da&ubreve;ra atento:
      <ekz>
        <tld/>i la astrojn<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i kvaza&ubreve; per linkaj okuloj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la filozofo konsumas sian vivon por <tld/>i la homojn;
      </ekz>
      <ekz>
        <tld/>adi iun
        <klr>(severe gardi sub sia rigardo)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekzame.0i">ekzameni</ref>,
      <ref cel="esplor.0i">esplori</ref>,
      <ref cel="konsid.0i">konsideri</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_z;&c_ib;&c_r;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_n;&c_a;&c_g;&c_l;&c_ja;&c_d;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">beobachten</trd>
    <trd lng="es">observar</trd>
    <trd lng="hu">megfigyel</trd>
    <trdgrp lng="nl">
      <trd>observeren</trd>,
      <trd>waarnemen</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>obserwowa&cacute;</trd>,
      <trd>przypatrywa&cacute; si&eogonek;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_n;&c_a;&c_b;&c_l;&c_ju;&c_d;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_l;&c_je;&c_d;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="observ.0i.obei">
    <dif>
      Akurate fari tion, kion ordonas le&gcirc;o, regulo ktp; ekzakte <ref
      tip="super" cel="obe.0i">obei</ref>:
      <ekz>
        mi <tld/>as Viajn ordonojn
        <fnt>
        <bib>MT</bib>,
        <lok>&Psa; 119:56</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i la teorian vidpunkton<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i severan elekton<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>u pli da ordo;
      </ekz>
      <ekz>
        <tld/>i la morojn de la patroj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>igi regulon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konfor">konformi&gcirc;i</ref>,
      <ref cel="plen.0umi">plenumi</ref>,
      <ref cel="sekv.0i">sekvi</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_k;&c_o;&c_n;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_y;&c_t;&c_r;&c_y;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_t;&c_r;&c_y;&c_m;&c_a;&c_c;&c_c;&c_a; <klr tip="amb">(&c_p;&c_r;&c_a;&c_v;&c_ib;&c_l;&c_a;&c_w; &c_ib; &c_d;&c_a;
      &c_g;.&c_p;.)</klr></trd>
    </trdgrp>
    <trd lng="de">beachten</trd>
    <trd lng="hu">betart</trd>
    <trd lng="nl">in <ind>acht</ind> nemen</trd>
    <trdgrp lng="pl">
      <trd>przestrzega&cacute;<klr> (prawa, obyczaje, itp.)</klr></trd>,
      <trd>zachowywa&cacute;<klr> (prawa, obyczaje, itp.)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_o;&c_b;&c_l;&c_ju;&c_d;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_b;&c_l;&c_ju;&c_s;&c_t;&c_i;</trd>;
      <trd>&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_p;&c_r;&c_a;&c_v;&c_i;&c_l;&c_a;&c_m; &c_i;
      &c_t;.&c_p;.)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="cs">pozorovat</trd>
  <trd lng="en">observe</trd>
  <trd lng="fr">observer</trd>
  <trd lng="pt">observar</trd>
  <trd lng="tp">lukin</trd>
</drv>

<drv mrk="observ.0ado">
  <kap><tld/>ado,
  <var><kap><tld/>o</kap></var></kap>
  <snc mrk="observ.0ado.rigardo">
    <dif>
      Atenta rigardado:
      <ekz>
        La <tld/>ado de la movoj de malamika trupo;
      </ekz>
      <ekz>
        la sciencoj bazitaj sur la <tld/>o.
      </ekz>
      <trdgrp lng="be">
        <trd>&c_n;&c_a;&c_z;&c_ib;&c_r;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
        <trd>&c_n;&c_a;&c_g;&c_l;&c_ja;&c_d;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
      </trdgrp>
      <trd lng="de">Beobachtung</trd>
      <trd lng="fr">observation<klr> (action)</klr></trd>
      <trd lng="hu">megfigyel&eacute;s</trd>
      <trd lng="nl">waarneming</trd>
      <trd lng="pl">obserwacja</trd>
      <trdgrp lng="pl">
        <trd>obserwacja</trd>,
        <trd>obserwowanie</trd>
      </trdgrp>
      <trd lng="ru">&c_n;&c_a;&c_b;&c_l;&c_ju;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
    </dif>
  </snc>
  <snc mrk="observ.0ado.obeo">
    <dif>
      Akurata obeado:
      <ekz>
        Prizorgi la severan <tld/>adon de la regularo.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_k;&c_a;&c_n;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_p;&c_r;&c_y;&c_t;&c_r;&c_y;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trd lng="de">Beachtung</trd>
    <trd lng="es">observaci&oacute;n</trd>
    <trd lng="fr">observance</trd>
    <trd lng="hu">betart&aacute;s</trd>
    <trd lng="nl">inachtneming</trd>
    <trdgrp lng="pl">
      <trd>zachowywane<klr> (prawa, obyczaju, itp.)</klr></trd>,
      <trd>prestrzeganie<klr> (prawa, obyczaju, itp.)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_o;&c_b;&c_l;&c_ju;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <trd lng="pt">observa&ccedil;&atilde;o</trd>
</drv>

<drv mrk="observ.0ajxo">
  <kap><tld/>a&jcirc;o,
  <var><kap><tld/>o</kap></var></kap>
  <snc mrk="observ.0ajxo.rigardajxo">
    <dif>
      &Ccirc;io, kion oni rimarkis a&ubreve; notis <tld/>ante:
      <ekz>
        kolekto da <tld/>a&jcirc;oj pri la tifoida febro.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_z;&c_ib;&c_r;&c_a;&c_n;&c_mol;&c_n;&c_je; <klr tip="amb">(&c_n;&c_je;&c_sh;&c_t;&c_a;
  &c_a;&c_d;&c_z;&c_n;&c_a;&c_ch;&c_a;&c_n;&c_a;&c_je;)</klr></trd>
  <trd lng="de">Wahrnehmung</trd>
  <trd lng="es">observaci&oacute;n</trd>
  <trd lng="fr">observation<klr> (fait)</klr></trd>
  <trd lng="hu">&eacute;szrev&eacute;tel</trd>
  <trd lng="nl">waarneming</trd>
  <trdgrp lng="pl">
    <trd>obserwacja</trd>,
    <trd>spostrze&zdot;enie</trd>,
    <trd>zapisek z obserwacji</trd>
  </trdgrp>
  <trd lng="pt">observa&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_n;&c_a;&c_b;&c_l;&c_ju;&c_d;&c_je;&c_n;&c_i;&c_je; <klr>(&c_o;&c_t;&c_m;&c_je;&c_ch;&c_je;&c_n;&c_n;&c_y;&c_j;
  &c_f;&c_a;&c_k;&c_t;)</klr></trd>
</drv>

<drv mrk="observ.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="observ.0ejo.KOMUNE">
    <dif>
      Loko ta&ubreve;ga a&ubreve; aran&gcirc;ita por <tld/>ado:
      <ekz>
        mi staris konstante sur mia <tld/>ejo dum la tago,
        kaj sur mia gardejo mi staris tutajn noktojn
        <fnt><bib>MT</bib>, <lok>&Jes; 21:8</lok></fnt>.
      </ekz>
    </dif>
    <subsnc mrk="observ.0ejo.SCI">
      <uzo tip="fak">SCI</uzo>
      <dif>
        Scienca esplorejo lokita kaj ekipita
        por rekta <tld/>o de iu natura medio:
        <ekz>
          <ref tip="vid" cel="observ.mar0ejo.MAR">marobservejo</ref>.
        </ekz>
      </dif>
      <trd lng="pl">obserwatorium</trd>
    </subsnc>
    <subsnc mrk="observ.0ejo.POL">
      <uzo tip="fak">POL</uzo>
      <dif>
        Institucio komisiata pri <tld/>ado
        kaj raportado de iuj sociaj agadoj:
        <ekz>
          starigi lingvan <tld/>ejon kiu <tld/>u
          kaj denuncu la lingvajn maljusta&jcirc;ojn
          tra la mondo
          <fnt>
            <aut>V. Lutermano</aut>,
            <vrk><url
            ref="http://www.mas-eo.org/article.php3?id_article=71">
            Raporto pri la Plurcentra Monda Socia Forumo
            2006</url></vrk>,
            <lok>Monda Asembleo Socia, 2006</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
  </snc>
  <trd lng="fr">observatoire</trd>
</drv>

<drv mrk="observ.0emo">
  <kap><tld/>emo</kap>
  <snc mrk="observ.0emo.rigardo">
    <dif>
      Deziro kaj kapablo por <tld/>i, por atente
      kaj detale rigardi:
      <ekz>
        kun <tld/>emo de avarulo sekvis liaj okuloj
        la sortojn de la domo
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="fr">sens de l'observation</trd>
  </snc>
</drv>

<drv mrk="observ.0anto">
  <kap><tld/>anto,
  <var><kap><tld/>ulo</kap></var></kap>
  <snc mrk="observ.0anto.KOMUNE">
    <dif>
      Atenta, zorga rigardanto:
      <ekz>
        <tld/>anto povus rimarki, ke la viza&gcirc;o de la prokuratoro
        <klr>...</klr> esprimas malpaciencon
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 25a, p. 260a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="observ.0anto.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="diplom1.0o.POL">Diplomato</ref>, kiun
      registaro sendas al konferenco, en kiu &gcirc;i ne volas oficiale
      partopreni, sed pri kiu &gcirc;i volas esti informita.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_z;&c_ib;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_n;&c_a;&c_g;&c_l;&c_ja;&c_d;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Beobachter</trd>
  <trd lng="es">observador (sust.)</trd>
  <trd lng="fr">observateur<klr> (subst.)</klr></trd>
  <trd lng="hu">megfigyel&odblac;</trd>
  <trd lng="nl">waarnemer</trd>
  <trdgrp lng="pl">
    <trd>obserwator</trd>,
    <trd>obserwant</trd>,
    <trd>osoba &sacute;ci&sacute;le prestrzegaj&aogonek;ca prawa, obyczaju, przepisu,
    itp.</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_b;&c_l;&c_ju;&c_d;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
</drv>

<drv mrk="observ.kasx0i">
  <kap>ka&scirc;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="observ.kasx0i.rigardi">
    <dif>
      Nevidate <tld/>i,
      <ref tip="vid" cel="gvat.0i.sxtele">gvati</ref>,
      <ref tip="vid" cel="spion.0i">spioni</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_ch;&c_y;&c_c;&c_mol; 
    <klr tip="amb">(&c_u;&c_p;&c_o;&c_t;&c_a;&c_j;)</klr></trd>,
    <trd>&c_sh;&c_p;&c_ib;&c_jo;&c_n;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_p;&c_je;&c_g;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">spionieren</trd>
  <trd lng="es">espiar</trd>
  <trd lng="fr">espionner</trd>
  <trdgrp lng="hu">
    <trd>megles</trd>,
    <trd>leselkedik</trd>,
    <trd>k&eacute;mkedik</trd>
  </trdgrp>
  <trd lng="nl">spionneren</trd>
  <trdgrp lng="pl">
    <trd>podgl&aogonek;da&cacute;</trd>,
    <trd>&sacute;ledzi&cacute;</trd>,
    <trd>szpiegowa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">espionar</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_je;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_sh;&c_p;&c_i;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="observ.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="observ.mal0i.obei">
    <dif>
      Malobei al, rompi, transpa&scirc;i:
      <ekz>
        vi amba&ubreve; fari&gcirc;os senvaloraj dibo&ccirc;uloj, mal<tld/>antaj la
        regulojn
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 75a, p. 592a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="respek.mal0i">malrespekti</ref>,
      <ref cel="ofend.0i.ion">ofendi<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_u;&c_sh;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_je; &c_t;&c_r;&c_y;&c_m;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de">mi&szlig;achten</trd>
  <trdgrp lng="hu">
    <trd>&aacute;th&aacute;g</trd>,
    <trd>megs&eacute;rt <klr tip="ind">
    (el&odblac;&iacute;r&aacute;st)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>transgredir</trd>,
    <trd>violar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>enfreindre</trd>,
    <trd>transgresser</trd>,
    <trd>violer<klr> (enfreindre)</klr></trd>
  </trdgrp>
  <trd lng="nl">overtreden</trd>
  <trdgrp lng="pl">
    <trd>nie przestrzega&cacute;<klr> (prawa, zwyczaju, itp.)</klr></trd>,
    <trd>nie zachowywa&cacute;<klr> (prawa, zwyczaju, itp.)</klr></trd>,
    <trd>&lstroke;ama&cacute;<klr> (prawo, zwyczaj, itp.)</klr></trd>,
    <trd>narusza&cacute;<klr> (prawo, zwyczaj, itp.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_je; &c_s;&c_o;&c_b;&c_l;&c_ju;&c_d;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="observ.mar0ejo">
  <kap>mar<tld/>ejo</kap>
  <snc mrk="observ.mar0ejo.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      <ref tip="super" cel="observ.0ejo.SCI"><tld lit="O"/>ejo</ref>
      de la mara medio:
      <ekz>
        li transprenis gvidan postenon en la mar<tld/>ejo
        &leftquot;Deutsche Seewarte&rightquot;
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Alfred_Wegener">
          Alfred Wegener</url>, 2011-03-24</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">observatoire marin</trd>
</drv>

<drv mrk="observ.stel0ejo">
  <kap>stel<tld/>ejo,
  <var><kap>astronomia <tld/>ejo</kap></var></kap>
  <snc mrk="observ.stel0ejo.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="super" cel="observ.0ejo.SCI"><tld lit="O"/>ejo</ref>
      ekipita por esploro de steloj kaj aliaj kosmaj apera&jcirc;oj:
      <ekz>
        tagmeze mi iris al la urba stel<tld/>ejo,
        por spekti tie la suneklipson
        <fnt>
          <aut>M. Blahu&scaron;</aut>,
          <vrk><url ref="http://blahma.livejournal.com/6266.html">
          Suneklipsa spektado...</url></vrk>,
          <lok>2006-03-30</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lasitaj &scirc;tonaj konstrua&jcirc;oj fundamentis mitojn kaj legendon 
        kaj diversspecan interpretadon de ilia utiligado kiel 
        astronomiaj <tld/>ejoj a&ubreve; lokoj sanktaj
        <fnt>
          <aut>Pola Radio</aut>,
          <vrk><url
          ref="http://www2.polskieradio.pl/eo/dokument.aspx?iid=130732">
          Ni invitas al Pollando: W&eogonek;siory kaj Czarna Woda</url></vrk>,
          <lok>2010-04-30</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">observatoire astronomique</trd>
  <trd lng="pl">obserwatorium astronomiczne</trd>
</drv>

<drv mrk="observ.veter0ejo">
  <kap>veter<tld/>ejo,
  <var><kap>meteologia <tld/>ejo</kap></var></kap>
  <snc mrk="observ.veter0ejo.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      <ref tip="super" cel="observ.0ejo.SCI"><tld lit="O"/>ejo</ref>
      registranta kaj esploranta veterajn apera&jcirc;ojn:
      <ekz>
        proksima veter<tld/>ejo tiam indikis temperaturon je minus 15 
        gradoj
        <fnt>
          <aut>H. Laine</aut>,
          <vrk><url
          ref="http://www.ipernity.com/doc/harri/6970314/in/album/115344">
          Frosta nesto</url></vrk>,
          <lok>2010-01-02</lok>
        </fnt>;
      </ekz>
      <ekz>
        Le Verrier tiam establis reton de meteologiaj <tld/>ejoj sur la 
        franca teritorio, celante averti la maristojn pri la &scirc;tormoj
        <fnt>
          <bib>Viki</bib>,
          <lok><url
          ref="http://eo.wikipedia.org/wiki/Urbain_Le_Verrier">
          Urbain Le Verrier</url>, 2011-04-27</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">observatoire m&eacute;t&eacute;orologique</trd>
  <trd lng="pl">obserwatorium meteorologiczne</trd>
</drv>
</art>
