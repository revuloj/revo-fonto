<drv mrk="observ.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="observ.0ejo.KOMUNE">
    <dif>
      Loko ta&ubreve;ga a&ubreve; aran&gcirc;ita por <tld/>ado:
      <ekz>
        mi staris konstante sur mia <tld/>ejo dum la tago,
        kaj sur mia gardejo mi staris tutajn noktojn
        <fnt><bib>MT</bib>, <lok>&Jes; 21:8</lok></fnt>;
      </ekz>
      <ekz>
        li tuj decidi&gcirc;is &scirc;an&gcirc;i sian <tld/>ejon, kaj loki sin apud la &ccirc;efpordego de la
        palaco
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>&ccirc;as<tld/>ejo</ind>
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 6 (104)</lok>
        </fnt>;
        <trdgrp lng="de">
          <trd>Ansitz</trd>,
          <trd>Anstand <klr>(Jagd)</klr></trd>,
          <trd>Hochsitz</trd>
        </trdgrp>
      </ekz>
      <ekz>
        korsara<klr>[n]</klr> &scirc;ipo<klr>[n]</klr> el Al&gcirc;erio <klr>[&#x2026;]</klr> la <tld/>ejo
        signalas al ni
        <fnt>
          <bib>DKM</bib>
          <lok>&Ccirc;apitro LXIII</lok>
        </fnt>.
      </ekz>
    </dif>
    <subsnc mrk="observ.0ejo.SCI">
      <uzo tip="fak">SCI</uzo>
      <dif>
        Scienca esplorejo lokita kaj ekipita
        por rekta <tld/>o de iu natura medio:
        <ekz>
          astronomia <tld/>ejo
          <klr>(vd <ref cel="observ.stel0ejo">stelobservejo</ref>)</klr>
          <fnt>
            <bib>ChL</bib>
            <lok>&Ccirc;apitro Dekdua</lok>
          </fnt>;
        </ekz>
        <ekz>
          la aerologia <tld/>ejo de Tateno
          <fnt>
            <bib>EeP</bib>
            <lok>Dua Parto</lok>
          </fnt>;
        </ekz>
        <ekz>
          <tld/>ejoj ebligas spekti (sen &gcirc;eni) la &ccirc;iutagan vivon de birdoj
          <fnt>
            <bib>Monato</bib>
            <aut>Jean-Yves Santerre</aut>
            <vrk><url ref="&Monato;/2013/010991.php">Kie akvo kaj &ccirc;ielo najbaras</url></vrk>
            <lok>2013</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="pl">obserwatorium</trd>
    </subsnc>
    <subsnc mrk="observ.0ejo.POL">
      <uzo tip="fak">POL</uzo>
      <dif>
        <ref tip="super" cel="instit.0o">Institucio</ref> komisiata pri <tld/>ado
        kaj raportado de iuj sociaj agadoj:
        <ekz>
          redaktoroj de <nom>Le Monde Diplomatique</nom> <klr>[&#x2026;]</klr> sugestis iun &leftquot;internacian
          <tld/>ejon de la amaskomunikiloj&rightquot;
          <fnt>
            <bib>Monato</bib>
            <aut>Gilbert LEDON</aut>
            <vrk><url ref="&Monato;/2003/008040.html">Kvina povo</url></vrk>
            <lok>2003</lok>
          </fnt>;
        </ekz>
        <ekz>
          starigi lingvan <tld/>ejon kiu <tld/>u
          kaj denuncu la lingvajn maljusta&jcirc;ojn
          tra la mondo
          <fnt>
            <aut>V. Lutermano</aut>,
            <vrk><url ref="http://www.mas-eo.org/article.php3?id_article=71">
            Raporto pri la Plurcentra Monda Socia Forumo
            2006</url></vrk>,
            <lok>Monda Asembleo Socia, 2006</lok>
          </fnt>;
        </ekz>
        <ekz>
          la <tld lit="O"/>ejo pri Rasismo kaj Ksenofobio en E&ubreve;ropo
          <fnt>
            <bib>Monato</bib>
            <aut>Marko Naoki Lins</aut>
            <vrk><url ref="&Monato;/2005/008603.php">Toleremo ankora&ubreve; ne tolerata</url></vrk>
            <lok>2005</lok>
          </fnt>;
        </ekz>
        <ekz>
          raporto komisiita de la konstanta <tld/>ejo pri mortoj interne de malliberejoj
          <fnt>
            <bib>Monato</bib>
            <aut>Roberto Pigro</aut>
            <vrk><url ref="&Monato;/2010/010161.php">Rajto je minimuma digno</url></vrk>
            <lok>2010</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
  </snc>
  <trd lng="cs">rozhledna</trd>
  <trd lng="fr">observatoire</trd>
  <trdgrp lng="ja">
    <trd>&#x89B3;&#x6E2C;&#x6240;  <pr>&#x304B;&#x3093;&#x305D;&#x304F;&#x3058;&#x3087;</pr></trd>,
    <trd>&#x76E3;&#x8996;&#x6240;  <pr>&#x304B;&#x3093;&#x3057;&#x3058;&#x3087;</pr></trd>
  </trdgrp>
  <trd lng="ro">observator</trd>
