<drv mrk="observ.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="observ.0i.rigardi">
    <dif>
      <ref tip="super" cel="rigard.0i">Rigardi</ref> ion ade,
      kun da&ubreve;ra atento:
      <ekz>
        <tld/>i la astrojn kaj mezuri la tempon
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la mastrino <klr>[&#x2026;]</klr> <tld/>is kvaza&ubreve; per linkaj okuloj &ccirc;iujn
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la libroj movis lian fantazion kaj igis lin tiamaniere <tld/>i la homojn
        <fnt>
          <bib>HsT</bib>
          <lok>La nepo</lok>
        </fnt>;
      </ekz>
      <ekz>
        de la plej proksima monteto <klr>[li]</klr> <tld/>is la batalon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        de tie li povis bone <tld/>adi la malgrandan elegantan sinjorinon
        <fnt>
          <bib>Fab1</bib>
          <lok>Persista stana soldato</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekzame.0i">ekzameni</ref>,
      <ref cel="esplor.0i">esplori</ref>,
      <ref cel="gvat.0i">gvati</ref>,
      <ref cel="konsid.0i">konsideri</ref>,
      <ref cel="spekt.0i">spekti</ref>,
      <ref cel="spion.0i">spioni</ref>,
      <ref cel="vacx.0i">va&ccirc;i</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_z;&c_ib;&c_r;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_n;&c_a;&c_g;&c_l;&c_ja;&c_d;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">beobachten</trd>
    <trd lng="es">observar</trd>
    <trd lng="hu">megfigyel</trd>
    <trdgrp lng="nl">
