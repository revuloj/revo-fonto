<drv mrk="observ.0ado">
  <kap><tld/>ado,
  <var><kap><tld/>o</kap></var></kap>
  <snc mrk="observ.0ado.rigardo">
    <dif>
      Atenta rigardado:
      <ekz>
        La <tld/>ado de la movoj de malamika trupo;
      </ekz>
      <ekz>
        la sciencoj bazitaj sur la <tld/>o.
      </ekz>
      <trdgrp lng="be">
        <trd>&c_n;&c_a;&c_z;&c_ib;&c_r;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
        <trd>&c_n;&c_a;&c_g;&c_l;&c_ja;&c_d;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
      </trdgrp>
      <trd lng="de">Beobachtung</trd>
      <trd lng="fr">observation<klr> (action)</klr></trd>
      <trd lng="hu">megfigyel&eacute;s</trd>
      <trd lng="nl">waarneming</trd>
      <trdgrp lng="pl">
        <trd>obserwacja</trd>,
        <trd>obserwowanie</trd>
      </trdgrp>
      <trdgrp lng="ro">
        <trd>observare</trd>,
        <trd>observarea</trd>
      </trdgrp>
      <trd lng="ru">&c_n;&c_a;&c_b;&c_l;&c_ju;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
    </dif>
  </snc>
  <snc mrk="observ.0ado.obeo">
    <dif>
      Akurata obeado:
      <ekz>
        Prizorgi la severan <tld/>adon de la regularo.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_k;&c_a;&c_n;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_p;&c_r;&c_y;&c_t;&c_r;&c_y;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trd lng="de">Beachtung</trd>
    <trd lng="es">observaci&oacute;n</trd>
    <trd lng="fr">observance</trd>
    <trd lng="hu">betart&aacute;s</trd>
    <trd lng="nl">inachtneming</trd>
    <trdgrp lng="pl">
      <trd>zachowywanie<klr> (prawa, obyczaju, itp.)</klr></trd>,
      <trd>przestrzeganie<klr> (prawa, obyczaju, itp.)</klr></trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>comportare<klr> (obicei)</klr></trd>,
      <trd>respectarea<klr> (obicei)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_o;&c_b;&c_l;&c_ju;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>dodr&#x17E;en&#xED;</trd>,
    <trd>pozorov&#xE1;n&#xED;</trd>,
    <trd>sledov&#xE1;n&#xED;</trd>,
    <trd>zachov&#xE1;n&#xED;</trd>,
    <trd>zkoum&#xE1;n&#xED;</trd>
  </trdgrp>
  <trd lng="pt">observa&ccedil;&atilde;o</trd>
  <trd lng="ro">observa&#x21B;ie</trd>
  <trdgrp lng="sk">
    <trd>pozorovanie</trd>,
    <trd>sledovanie <klr>(&#x10D;innos&tcaron;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="observ.0ajxo">
  <kap><tld/>a&jcirc;o,
  <var><kap><tld/>o</kap></var></kap>
  <snc mrk="observ.0ajxo.rigardajxo">
