<drv mrk="observ.mar0ejo">
  <kap>mar<tld/>ejo</kap>
  <snc mrk="observ.mar0ejo.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      <ref tip="super" cel="observ.0ejo.SCI"><tld lit="O"/>ejo</ref>
      de la mara medio:
      <ekz>
        li transprenis gvidan postenon en la mar<tld/>ejo
        &leftquot;<nom>Deutsche Seewarte</nom>&rightquot;
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Alfred_Wegener">
          Alfred Wegener</url>, 2011-03-24</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">observatoire marin</trd>
  <trd lng="pl">obserwatorium morskie</trd>
  <trd lng="ro">observatorul maritim</trd>
</drv>

<drv mrk="observ.stel0ejo">
  <kap>stel<tld/>ejo,
  <var><kap>astronomia <tld/>ejo</kap></var></kap>
  <snc mrk="observ.stel0ejo.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="super" cel="observ.0ejo.SCI"><tld lit="O"/>ejo</ref>
      ekipita por esploro de steloj kaj aliaj kosmaj apera&jcirc;oj:
      <ekz>
        tagmeze mi iris al la urba stel<tld/>ejo,
        por spekti tie la suneklipson
        <fnt>
          <aut>M. Blahu&scaron;</aut>,
          <vrk><url ref="http://blahma.livejournal.com/6266.html">
          Suneklipsa spektado...</url></vrk>,
          <lok>2006-03-30</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lasitaj &scirc;tonaj konstrua&jcirc;oj fundamentis mitojn kaj legendon
        kaj diversspecan interpretadon de ilia utiligado kiel
        astronomiaj <tld/>ejoj a&ubreve; lokoj sanktaj
        <fnt>
          <aut>Pola Radio</aut>,
          <vrk><url ref="http://www2.polskieradio.pl/eo/dokument.aspx?iid=130732">
          Ni invitas al Pollando: W&eogonek;siory kaj Czarna Woda</url></vrk>,
          <lok>2010-04-30</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_s;&c_e;&c_r;&c_v;&c_a;&c_t;&c_o;&c_r;&c_y;&c_ja; <klr>(&c_a;&c_s;&c_t;&c_r;&c_a;&c_n;&c_a;&c_m;&c_ib;&c_ch;&c_n;&c_a;&c_ja;, &c_a;&c_s;&c_t;&c_r;&c_a;&c_f;&c_ib;&c_z;&c_y;&c_ch;&c_n;&c_a;&c_ja;)</klr></trd>
  <trd lng="cs">hv&ecaron;zd&aacute;rna</trd>
  <trd lng="fr">observatoire astronomique</trd>
  <trd lng="pl">obserwatorium astronomiczne</trd>
  <trd lng="ro">observator astronomic</trd>
  <trd lng="sk">hvezd&aacute;re&ncaron;</trd>
  <trdgrp lng="zh">
    <trd>&#x76D1;&#x6D4B;&#x6240; <pr>ji&#x101;nc&#xE8;s&#x1D4;p</pr></trd>,
    <trd>&#x76E3;&#x6E2C;&#x6240; <pr>ji&#x101;nc&#xE8;s&#x1D4;p</pr></trd>
  </trdgrp>
</drv>

<drv mrk="observ.veter0ejo">
  <kap>veter<tld/>ejo,
  <var><kap>meteologia <tld/>ejo</kap></var></kap>
  <snc mrk="observ.veter0ejo.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      <ref tip="super" cel="observ.0ejo.SCI"><tld lit="O"/>ejo</ref>
      registranta kaj esploranta veterajn apera&jcirc;ojn:
      <ekz>
        proksima veter<tld/>ejo tiam indikis temperaturon je minus 15
        gradoj
        <fnt>
          <aut>H. Laine</aut>,
          <vrk><url ref="http://www.ipernity.com/doc/harri/6970314/in/album/115344">
          Frosta nesto</url></vrk>,
          <lok>2010-01-02</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Le Verrier</nom> tiam establis reton de meteologiaj <tld/>ejoj sur la
        franca teritorio, celante averti la maristojn pri la &scirc;tormoj
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Urbain_Le_Verrier">
          Urbain Le Verrier</url>, 2011-04-27</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_s;&c_e;&c_r;&c_v;&c_a;&c_t;&c_o;&c_r;&c_y;&c_ja; <klr>(&c_m;&c_e;&c_t;&c_e;&c_a;&c_r;&c_a;&c_l;&c_ja;&c_g;&c_ib;&c_ch;&c_n;&c_a;&c_ja;)</klr></trd>
  <trd lng="fr">observatoire m&eacute;t&eacute;orologique</trd>
