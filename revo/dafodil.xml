<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: dafodil.xml,v 1.1 2025/03/20 18:36:37 afido Exp $">
    <kap>
      <rad>dafodil</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="dafodil.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ore flava <ref tip="super" cel="narcis.0o">narciso</ref>
      (<trd lng="la">Narcissus pseudonarcissus</trd>) kun granda tubforma
      kromkorolo kaj bulbo:
      <ekz>
        specioj &gcirc;ardene kulturataj, interalie jonkvilo, tazeto, <tld/>o kaj poetnarciso
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=246267"
              >Narciso (floro)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        sekvas dalio, kaj poste dafodilo <klr>[&#8230;]</klr> &leftquot;mirinde!&rightquot; krietis Doroteo
        <fnt>
          <aut>Lyman Frank Baum</aut>
          <vrk><url ref="https://eduinf.waw.pl/esp/lern/librejo/0079/0007.php"
              >La Magio de Oz</url></vrk>
          <lok>&ccirc;apitro 5, Feli&ccirc;a Angulo de Oz</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/4/40/Narcissus_pseudonarcissus1.jpg/360px-Narcissus_pseudonarcissus1.jpg"
      prm="CC BY-SA 3.0"
      ><tld/>o
        <fnt>
          <lok><url ref="&WCW;/File:Narcissus_pseudonarcissus1.jpg"
              >https://commons.wikimedia.org/wiki/File:Narcissus_pseudonarcissus1.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="de">
      <trd>Osterglocke</trd>,
      <trd>Gelbe Narzisse</trd>
    </trdgrp>
  </snc>
</drv>

  </art>

