<art mrk="$Id: kalcin.xml,v 1.20 2022/03/21 17:51:56 revo Exp $">
<kap>
   <rad>kalcin</rad>/i
</kap>
<drv mrk="kalcin.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc>
     <dif>
       Cindrigi per forta varmigo nefandeblajn substancojn:
       <ekz><tld/>ita magnezo.</ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_p;&c_r;&c_a;&c_p;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_n;&c_a;&c_p;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_k;&c_a;&c_l;&c_mol;&c_c;&c_y;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
   </trdgrp>
  <trd lng="ca">calcinar</trd>
  <trdgrp lng="cs">
    <trd>kalcinovat</trd>,
    <trd>pra&zcaron;it</trd>,
    <trd>vy&zcaron;&iacute;hat</trd>,
    <trd>&zcaron;&iacute;hat</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>kalzinieren</trd>,
      <trd>zu <ind>Kalk</ind> brennen</trd>,
      <trd>veraschen</trd>
   </trdgrp>
  <trd lng="en">calcine</trd>
  <trd lng="es">calcinar</trd>
  <trd lng="fa">&a_t;&f_k;&a_l;&f_y;&a_s; &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="fi">
     <trd>kalsinoida</trd>,
     <trd>hehkuttaa <klr tip="amb"> (tekn.)</klr></trd>
   </trdgrp>
  <trdgrp lng="fr">
      <trd>calciner</trd>,
      <trd>r&eacute;duire en cendres</trd>
   </trdgrp>
  <trd lng="hu">kalcin&aacute;l</trd>
  <trdgrp lng="nl">
     <trd>calcineren</trd>,
     <trd>uitgloeien</trd>
   </trdgrp>
  <trdgrp lng="pl">
     <trd>wypra&zdot;a&cacute;</trd>,
     <trd>wypala&cacute;</trd>
   </trdgrp>
  <trd lng="pt">calcinar</trd>
  <trdgrp lng="ru">
     <trd>&c_p;&c_r;&c_o;&c_k;&c_a;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_o;&c_b;&c_zh;&c_i;&c_g;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_k;&c_a;&c_l;&c_mol;&c_c;&c_i;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
   </trdgrp>
  <trd lng="sk">kalcinova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x430;&#x43B;&#x44C;&#x446;&#x438;&#x43D;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x440;&#x43E;&#x436;&#x430;&#x440;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x438;&#x43F;&#x430;&#x43B;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>
</art>
