<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&hcirc;imer</rad>/o
</kap>
<drv mrk="hximer.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="hximer.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Mitologia monstro, kies bu&scirc;oj vomis flamojn.
    </dif>
    <refgrp tip="vid">
      <ref cel="hidr.0o.MIT">hidro</ref>.
    </refgrp>
    <trd lng="nl">chim&egrave;re</trd>
    <trd lng="ca">quimera</trd>
    <trd lng="hu">Kim&eacute;ra</trd>
    <trd lng="pt">quimera</trd>
    <trd lng="ru">&c_h;&c_i;&c_m;&c_je;&c_r;&c_a;</trd>
  </snc>
  <snc mrk="hximer.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Neefektivigebla utopio:
      <ekz>
        longtempe multaj kredis, ke artefarita lingvo estas <tld/>o;
      </ekz>
      <ekz>
        <tld/>a projekto<fnt>B</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="iluzi.0o">iluzio</ref>,
      <ref cel="rev">revo</ref>,
      <ref cel="fantaz">fantazia&jcirc;o</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="aer.0a">aera</ref>,
      <ref cel="kastel.0o">kastelo</ref>,
      <ref cel="dorlot.0i">dorloti</ref>,
      <ref cel="lul.0i">luli</ref>.
    </refgrp>
    <trd lng="nl">hersenschim</trd>
  </snc>
  <trd lng="de">Chim&auml;re</trd>
  <trd lng="en">chimera</trd>
  <trd lng="es">quimera</trd>
  <trd lng="fr">chim&egrave;re</trd>
  <trdgrp lng="hu">
    <trd>agyr&eacute;m</trd>,
    <trd>&aacute;br&aacute;ndk&eacute;p</trd>
  </trdgrp>
  <trd lng="pl">chimera</trd>
  <trd lng="pt">quimera</trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_i;&c_m;&c_je;&c_r;&c_a;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_b;&c_y;&c_t;&c_o;&c_ch;&c_n;&c_a;&c_ja; &c_m;&c_je;&c_ch;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">chim&eacute;ra</trd>
</drv>
</art>
