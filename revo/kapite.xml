<kap>
  <ofc>*</ofc>
  <rad>kapitel</rad>/o
</kap>
<drv mrk="kapite.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ARKI</uzo>
  <snc>
    <dif>
      Elstara supra ekstremo de <ref tip="malprt" cel="kolon.0o.ARKI">kolono</ref> ofte skulpte ornamita:
      <ekz>
        dorika, jonika, korinta <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_p;&c_ib;&c_t;&c_e;&c_l;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>hlavice sloupu</trd>,
    <trd>kapitel</trd>,
    <trd>kapitulum</trd>
  </trdgrp>
  <trd lng="de">Kapitell</trd>
  <trd lng="en">capital <klr>(head of pillar or pilaster)</klr></trd>
  <trd lng="es">capitel</trd>
  <trdgrp lng="fr">
    <trd>chapiteau<klr tip="ind"> (de colonne)</klr></trd>
  </trdgrp>
  <trd lng="hu">oszlopf&odblac;</trd>
  <trd lng="ja">&#x67F1;&#x982D;  <pr>&#x3061;&#x3085;&#x3046;&#x3068;&#x3046;</pr></trd>
  <trd lng="nl">kapiteel</trd>
  <trdgrp lng="pl">
    <trd>kapitel</trd>
    <trd>g&lstroke;owica</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_p;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">hlavica st&#x13A;pu</trd>
  <trd lng="uk">&#x43A;&#x430;&#x43F;&#x456;&#x442;&#x435;&#x43B;&#x44C;</trd>
</drv>
</art>
