      <ekz>
        dorika, jonika, korinta <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Kapitell</trd>
  <trd lng="en">capital <klr>(head of pillar or pilaster)</klr></trd>
  <trd lng="nl">kapiteel</trd>
  <trd lng="ru">&c_k;&c_a;&c_p;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
</drv>
</art>
