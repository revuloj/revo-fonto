<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pantom.xml,v 1.24 2026/02/12 04:00:21 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>pantomim</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="pantom.0o">
  <kap><tld/>o</kap>
  <snc mrk="pantom.0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="super" cel="teatr.0ajxo.TEA">Teatra&jcirc;o</ref>, en kiu la
      personoj rolas nur per gestoj, ne parolante:
      <ekz>
        tio estas la vivo mem, respeguli&gcirc;anta en etaj scenoj kaj 
        <tld/>oj, alloganta &ccirc;i tien la publikon
        <fnt>
          <aut>F. Ko&zcaron;&iacute;k, trad. J. Karen kaj J. Vondrou&scaron;ek</aut>,
          <vrk>La plej eminenta inter Pierotoj</vrk>,
          <lok>2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_n;&c_t;&c_a;&c_m;&c_ib;&c_m;&c_a;</trd>
  <trd lng="bg">&c_p;&c_a;&c_n;&c_t;&c_o;&c_m;&c_i;&c_m;&c_a;</trd>
  <trd lng="ca">pantomima</trd>
  <trdgrp lng="cs">
    <trd>n&ecaron;mohra</trd>,
    <trd>pantomima</trd>
  </trdgrp>
  <trd lng="de">Pantomime</trd>
  <trdgrp lng="en">
    <trd>pantomime</trd>,
    <trd>dumb show</trd>,
    <trd>dumbshow</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>mime<klr tip="amb"> (pantomime)</klr></trd>,
    <trd>pantomime</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>n&eacute;maj&aacute;t&eacute;k</trd>,
    <trd>pantomim</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12497;&#12531;&#12488;&#12510;&#12452;&#12512;</trd>,
    <trd>&#28961;&#35328;&#21127;  <pr>&#12416;&#12372;&#12435;&#12370;&#12365;</pr></trd>
  </trdgrp>
  <trd lng="nl">pantomime</trd>
  <trdgrp lng="pl">
    <trd>pantomima</trd>,
    <trd>mimodram</trd>
  </trdgrp>
  <trd lng="pt">pantomima</trd>
  <trd lng="ro">pantomim&abreve;</trd>
  <trd lng="ru">&c_p;&c_a;&c_n;&c_t;&c_o;&c_m;&c_i;&c_m;&c_a;</trd>
  <trd lng="sk">nemohra</trd>
  <trd lng="uk">&c_p;&c_a;&c_n;&c_t;&c_o;&c_m;&c_ib;&c_m;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#21713;&#21095; <pr>y&#462;j&ugrave;</pr></trd>,
    <trd>&#21854;&#21127; <pr>y&#462;j&ugrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pantom.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="pantom.0isto.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="super" cel="aktor.0o.TEA">Aktoro</ref> ludanta <tld/>on:
      <ekz>
        &leftquot;ho, estas feli&ccirc;ege, baptopatro,&rightquot;
	kriis Ruby, preska&ubreve; dancante;
	&leftquot;mi estos kolumbino kaj vi estu <tld/>isto&rightquot;
        <fnt>
          <aut>G. K. Chesterton, trad. C. Bean</aut>,
          <vrk>La Naiveco de Pastro Brown</vrk>,
          <lok>1937</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_n;&c_t;&c_a;&c_m;&c_ib;&c_m;&c_ib;&c_s;&c_t;</trd>
  <trd lng="bg">&c_p;&c_a;&c_n;&c_t;&c_o;&c_m;&c_i;&c_m;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>mim<klr tip="amb"> (actor)</klr></trd>,
    <trd>pantomim</trd>
  </trdgrp>
  <trd lng="cs">mim</trd>
  <trd lng="de">Geb&auml;rdenspieler</trd>
  <trdgrp lng="en">
    <trd>mime</trd>,
    <trd>mime artist</trd>
  </trdgrp>
  <trd lng="fr">mime<klr tip="amb"> (acteur)</klr></trd>
  <trdgrp lng="hu">
    <trd>pantomimj&aacute;t&eacute;kos</trd>,
    <trd>pantomimes</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12497;&#12531;&#12488;&#12510;&#12452;&#12512;&#20467;&#20778;  <pr>&#12497;&#12531;&#12488;&#12510;&#12452;&#12512;&#12399;&#12356;&#12422;&#12358;</pr></trd>,
    <trd>&#28961;&#35328;&#21127;&#24441;&#32773;  <pr>&#12416;&#12372;&#12435;&#12370;&#12365;&#12420;&#12367;&#12375;&#12419;</pr></trd>
  </trdgrp>
  <trd lng="nl">pantomimespeler</trd>
  <trd lng="pl">mim</trd>
  <trd lng="ro">mim&abreve;</trd>
  <trd lng="ru">&c_p;&c_a;&c_n;&c_t;&c_o;&c_m;&c_i;&c_m;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">m&iacute;m</trd>
  <trd lng="uk">&c_m;&c_ib;&c_m;</trd>
  <trdgrp lng="zh">
    <trd>&#40664;&#21095; <pr>m&ograve;j&ugrave;</pr></trd>,
    <trd>&#40664;&#21127; <pr>m&ograve;j&ugrave;</pr></trd>
  </trdgrp>
</drv>
</art>
