          <aut>H. Sienkiewicz, trad. Lidia Zamenhof</aut>
          <vrk>Quo Vadis</vrk>
          <lok>1933</lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_t;&c_y;&c_s;&c_ja;&c_ch;&c_a;&c_g;&c_o;&c_d;&c_z;&c_mol;&c_d;&c_z;&c_je;</trd>
