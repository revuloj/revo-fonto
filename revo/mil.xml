<drv mrk="mil.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="mil.0o.nombro">
    <dif>
      La nombro <tld/>.
    </dif>
    <trd lng="de">Tausend</trd>
  </snc>
  <snc>
    <dif>
      <tld lit="M"/> aferoj kune:
      <ekz>
        la tuta popolo eliris la&ubreve; centoj kaj <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 18:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        multaj <tld/>oj da <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 68:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvaza&ubreve; <tld/>oj da vitraj sonoriletoj ektintis
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        gro&scirc;on &scirc;telis &dash; ho &scirc;telisto! <tld/>ojn &scirc;telis &dash; financisto
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Tausender</trd>
  </snc>
<trd lng="be">&c_t;&c_y;&c_s;&c_ja;&c_ch;&c_a; <klr>(&c_n;&c_a;&c_z;.)</klr></trd>
</drv>

<drv mrk="mil.0ono">
  <kap>
    <tld/>ono
  </kap>
  <snc>
    <dif>
      La <tld/>a parto de io:
      <ekz>
        &ccirc;iuj milionoj konsistas el <tld/>onoj
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="mili1.0">mili-</ref>
  </snc>
  <trd lng="be">&c_t;&c_y;&c_s;&c_ja;&c_ch;&c_n;&c_a;&c_ja; &c_ch;&c_a;&c_s;&c_t;&c_k;&c_a;</trd>
  <trd lng="de">Tausendstel</trd>
</drv>

<drv mrk="mil.00">
  <kap><tld/><tld/></kap>
  <snc mrk="mil.00.KOMUNE">
    <dif>
      Multegaj, granda nombro da:
      <ekz>
        subite ekbrilas en la krepusketo la <tld/><tld/> artefaritaj
        sunoj de tiu &ccirc;i artefarita mondo; la urbo veki&gcirc;as
        <fnt>
          <aut>S. Engholm</aut>
          <vrk>Al Torento</vrk>
          <lok>1930</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_y;&c_s;&c_ja;&c_ch;&c_y; &c_ib; &c_t;&c_y;&c_s;&c_ja;&c_ch;&c_y;</trd>
  <trd lng="cs">tis&iacute;c</trd>
  <trd lng="fr">plein de</trd>
  <trd lng="it">migliaia<klr> (fig.)</klr></trd>
  <trd lng="pl">tysi&aogonek;ce</trd>
  <trd lng="sk">tis&iacute;c</trd>
  <trdgrp lng="zh">
    <trd>&#20805;&#20998; <pr>ch&omacron;ngf&egrave;n</pr></trd>,
    <trd>&#20805;&#30408; <pr>ch&omacron;ngy&iacute;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mil.00o">
  <kap><tld/><tld/>o</kap>
  <snc mrk="mil.00o.KOMUNE">
    <dif>
      <ref tip="super" cel="mult.0ego.KOMUNE">Multego</ref> da
      nombreblaj a&jcirc;oj:
      <ekz>
        <tld/><tld/>oj da sanktuloj
        <fnt><bib>MT</bib> <lok>&Rea; 33:2</lok></fnt>;
      </ekz>
      <ekz>
        jen brakumoj al <tld/><tld/>oj
        <fnt>
          <aut>F. Schiller, trad. K. Kalocsay</aut>
          <vrk>
<url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/np/np5802/ schiller.html">
          Al la &gcirc;ojo</url></vrk>
          <lok>Norda Prismo 1958:1, p. 65-66a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_z;&c_mol;&c_l;&c_ib;&c_ch;</trd>
  <trd lng="fr">multitude</trd>
  <trd lng="it">moltitudine</trd>
  <trd lng="pl">tysi&aogonek;ce</trd>
</drv>

