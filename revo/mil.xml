<kap>
   <ofc>*</ofc>
   <rad>mil</rad> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="mil.0">
   <kap><ofc>*</ofc><tld/></kap>
   <mlg>1000</mlg>
   <snc mrk="mil.0.MAT">
     <dif>
       Dekfoje cent:
       <ekz>
         <tld/> soldatoj;
       </ekz>
       <ekz>
         <tld/>o da soldatoj.
       </ekz>
     </dif>
     <ref tip="lst" cel="nombr.0o.MAT" lst="voko:nombroj" val="1000">nombro</ref>
   </snc>
   <trd lng="af">duisend</trd>
  <trd lng="am">&#x123A;&#x1205;</trd>
  <trd lng="ar">&#x623;&#x644;&#x641;</trd>
  <trd lng="az">min</trd>
  <trd lng="be">&c_t;&c_y;&c_s;&c_ja;&c_ch;&c_a;</trd>
   <trd lng="bg">&c_h;&c_i;&c_l;&c_ja;&c_d;&c_a;</trd>
   <trd lng="bn">&#x9B9;&#x9BE;&#x99C;&#x9BE;&#x9B0;</trd>
  <trd lng="bo">&#xF66;&#xF9F;&#xF7C;&#xF44;</trd>
  <trd lng="br">mil</trd>
   <trd lng="cs">tis&iacute;c</trd>
   <trd lng="da">tusinde</trd>
  <trd lng="de">tausend</trd>
   <trd lng="en">thousand</trd>
   <trd lng="es">mil</trd>
   <trd lng="fa">&a_h;&a_z;&a_A;&a_r;</trd>
   <trd lng="fi">tuhat</trd>
   <trd lng="fr">mille</trd>
   <trd lng="fy">t&ucirc;zen</trd>
   <trd lng="he">&alef;&lamed;&fpe;</trd>
   <trd lng="hu">ezer</trd>
   <trd lng="hy">&#x570;&#x561;&#x566;&#x561;&#x580;</trd>
  <trd lng="id">se<ind>ribu</ind></trd>
   <trd lng="io">mil</trd>
   <trd lng="it">mille</trd>
   <trd lng="ku">hezar</trd>
   <trd lng="ne">&#x939;&#x91C;&#x93E;&#x930;</trd>
  <trd lng="nl">duizend</trd>
   <trd lng="pl">tysi&aogonek;c</trd>
   <trd lng="pt">mil</trd>
   <trd lng="ro">o mie</trd>
   <trd lng="ru">&c_t;&c_y;&c_s;&c_ja;&c_ch;&c_a;</trd>
   <trd lng="sk">tis&iacute;c</trd>
   <trd lng="sq">mij&#xEB;</trd>
  <trd lng="sv">tusen</trd>
   <trd lng="tl">libo</trd>
  <trd lng="tr">bin</trd>
   <trd lng="vo">mil</trd>
   <trd lng="zh">&#x5343; [<ind>qi&#x101;n</ind>]</trd>
</drv>

<drv mrk="mil.00">
  <kap><tld/><tld/></kap>
  <snc mrk="mil.00.KOMUNE">
    <dif>
      Grandnombro da:
      <ekz>
        subite ekbrilas en la krepusketo la <tld/><tld/> artefaritaj 
        sunoj de tiu &ccirc;i artefarita mondo. La urbo veki&gcirc;as
        <fnt>
          <aut>S. Engholm</aut>,
          <vrk>Al Torento</vrk>, 
          <lok>1930</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">tis&iacute;c</trd>
  <trd lng="fr">plein de</trd>
  <trd lng="it">migliaia<klr> (fig.)</klr></trd>
  <trd lng="pl">tysi&aogonek;ce</trd>
  <trd lng="sk">tis&iacute;c</trd>
</drv>

<drv mrk="mil.00o">
  <kap><tld/><tld/>o</kap>
  <snc mrk="mil.00o.KOMUNE">
    <dif>
      <ref tip="super" cel="mult.0ego.KOMUNE">Multego</ref> da 
      nombreblaj a&jcirc;oj:
      <ekz>
        <tld/><tld/>oj da sanktuloj
        <fnt><bib>MT</bib>, <lok>&Rea; 33:2</lok></fnt>;
      </ekz>
      <ekz>
        jen brakumoj al <tld/><tld/>oj
        <fnt>
          <aut>F. Schiller, trad. K. Kalocsay</aut>,
          <vrk>
<url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/np/np5802/ schiller.html">
          Al la &gcirc;ojo</url></vrk>,
          <lok>Norda Prismo 1958:1, p. 65-66a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">multitude</trd>
  <trd lng="it">moltitudine</trd>
  <trd lng="pl">tysi&aogonek;ce</trd>
</drv>

<drv mrk="mil.jar0o">
   <kap>jar<tld/>o</kap>
   <snc>
     <dif>
       <tld lit="M"/>o da jaroj.
     </dif>
   </snc>
  <trd lng="be">&c_t;&c_y;&c_s;&c_ja;&c_ch;&c_a;&c_g;&c_o;&c_d;&c_z;&c_mol;&c_d;&c_z;&c_je;</trd>
  <trdgrp lng="bg">
     <trd>&c_h;&c_i;&c_l;&c_ja;&c_d;&c_o;&c_l;&c_je;&c_t;&c_i;&c_je;</trd>,
     <trd>&c_h;&c_i;&c_l;&c_ja;&c_d;&c_a; &c_g;&c_o;&c_d;&c_i;&c_n;&c_i;</trd>
   </trdgrp>
  <trd lng="br">milved</trd>
  <trd lng="cs">tis&iacute;cilet&iacute;</trd>
  <trd lng="de">Jahrtausend</trd>
  <trd lng="en">millenium</trd>
  <trd lng="es">milenio</trd>
  <trd lng="fa">&a_h;&a_z;&a_A;&a_r;&a_h;</trd>
  <trd lng="fi">vuosituhat</trd>
  <trd lng="fr">mill&eacute;naire</trd>
  <trd lng="fy">milennium</trd>
  <trdgrp lng="he">
     <trd>&alef;&lamed;&fpe;</trd>,
     <trd>&mem;&yod;&lamed;&nun;&yod;&vav;&fmem;</trd>
   </trdgrp>
  <trd lng="hu">&eacute;vezred</trd>
  <trd lng="id">milenium</trd>
  <trd lng="io">yarmilo</trd>
  <trd lng="it">millennio</trd>
  <trd lng="nl">millenium</trd>
  <trd lng="pl">tysi&aogonek;clecie</trd>
  <trd lng="ro">mileniu</trd>
  <trd lng="ru">&c_t;&c_y;&c_s;&c_ja;&c_ch;&c_je;&c_l;&c_je;&c_t;&c_i;&c_je;</trd>
  <trd lng="sk">tis&iacute;cro&ccaron;ie</trd>
  <trd lng="sv">&aring;rtusende</trd>
  <trd lng="tr">biny&inodot;l</trd>
  <trd lng="vo">milyel</trd>
</drv>
</art>
