<drv mrk="mil.00">
  <kap><tld/><tld/></kap>
  <snc mrk="mil.00.KOMUNE">
    <dif>
      Multegaj, granda nombro da:
      <ekz>
        subite ekbrilas en la krepusketo la <tld/><tld/> artefaritaj
        sunoj de tiu &ccirc;i artefarita mondo; la urbo veki&gcirc;as
        <fnt>
          <aut>S. Engholm</aut>
          <vrk>Al Torento</vrk>
          <lok>1930</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">tis&iacute;c</trd>
  <trd lng="fr">plein de</trd>
  <trd lng="it">migliaia<klr> (fig.)</klr></trd>
  <trd lng="pl">tysi&aogonek;ce</trd>
  <trd lng="sk">tis&iacute;c</trd>
  <trdgrp lng="zh">
    <trd>&#x5145;&#x5206; <pr>ch&#x14D;ngf&#xE8;n</pr></trd>,
    <trd>&#x5145;&#x76C8; <pr>ch&#x14D;ngy&#xED;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mil.00o">
  <kap><tld/><tld/>o</kap>
  <snc mrk="mil.00o.KOMUNE">
    <dif>
      <ref tip="super" cel="mult.0ego.KOMUNE">Multego</ref> da
      nombreblaj a&jcirc;oj:
      <ekz>
        <tld/><tld/>oj da sanktuloj
        <fnt><bib>MT</bib> <lok>&Rea; 33:2</lok></fnt>;
      </ekz>
      <ekz>
        jen brakumoj al <tld/><tld/>oj
        <fnt>
          <aut>F. Schiller, trad. K. Kalocsay</aut>
          <vrk>
<url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/np/np5802/ schiller.html">
          Al la &gcirc;ojo</url></vrk>
          <lok>Norda Prismo 1958:1, p. 65-66a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">multitude</trd>
  <trd lng="it">moltitudine</trd>
  <trd lng="pl">tysi&aogonek;ce</trd>
</drv>

<drv mrk="mil.jar0o">
