<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>feti&ccirc;</rad>/o
</kap>
<drv mrk="feticx.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Objekto respektegata de sova&gcirc;uloj kiel sidejo de magia povo,
      a&ubreve; kiel lo&gcirc;ejo de dia&jcirc;o. 
    </dif>
    <refgrp tip="sin">
      <ref cel="idol.0o">idolo</ref>.
    </refgrp>
    <trd lng="de">Fetisch</trd>
  </snc>
  <snc num="2">
    <dif>
      Objekto, kiun supersti&ccirc;aj homoj konsideras kiel bon&scirc;anciga. 
    </dif>
    <refgrp tip="sin">
      <ref cel="amulet.0o">amuleto</ref>,
      <ref cel="talism.0o">talismano</ref>.
    </refgrp>
    <trd lng="de">Ungl&uuml;ckszeichen</trd>
  </snc>
  <snc num="3">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Persono, konsiderata kiel netu&scirc;ebla, nekritikebla. 
    </dif>
    <trd lng="de">Gott</trd>
  </snc>
  <trd lng="es">fetiche</trd>
  <trd lng="ru">&c_f;&c_je;&c_t;&c_i;&c_sh;</trd>
</drv>
<drv mrk="feticx.0ismo">
  <kap><tld/>ismo</kap>
  <snc num="1">
    <dif>
      Religia rilato inter homo k <tld/>oj. 
    </dif>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Blinda admira fido al iu persono.  
    </dif>
  </snc>
  <trd lng="es">fetichismo</trd>
  <trd lng="ru">&c_f;&c_je;&c_t;&c_i;&c_sh;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
