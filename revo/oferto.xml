<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: oferto.xml,v 1.23 2023/10/20 13:11:47 revo Exp $">
<kap>
  <rad>ofertori</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="oferto.0o">
  <kap><tld/>o</kap>
  <snc mrk="oferto.0o.KAT">
    <uzo tip="fak">KAT</uzo>
    <dif>
      Tiu parto de la <ref tip="malprt" cel="mes.0o">meso</ref>
      en kiu la pastro prezentas (oferas) al Dio panon kaj vinon
      anta&ubreve; la konsekrado, kaj la pre&gcirc;oj akompanantaj
      tiun prezentadon:
      <ekz>
        eldrinkeje mi neniam alportis vinon por li,
        sed iom el tiu uzata dum la <tld/>o li gardis
        en la kofro
        <fnt>
        <aut>E. de Bonaero</aut>,
        <vrk>La Friponeto de Tormes</vrk>,
        <lok>1973</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">ofertori</trd>
  </snc>
  <snc mrk="oferto.0o.orto">
    <uzo tip="fak">KRI</uzo>
    <ref tip="dif" cel="prepar.0ado.liturgia">Preparado<sncref/>
    </ref>
  </snc>
  <trd lng="be">
  &c_p;&c_r;&c_y;&c_n;&c_ja;&c_s;&c_je;&c_n;&c_mol;&c_n;&c_je;
  &c_d;&c_a;&c_r;&c_o;&c_w;
  <klr>(&c_u; &c_ch;&c_a;&c_s; &c_ib;&c_m;&c_sh;&c_y;)</klr></trd>
  <trdgrp lng="ca">
    <trd>pr&ograve;tesi</trd>,
    <trd>proscom&iacute;dia</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ob&#x11B;tov&#xE1;n&#xED; <klr>(&#x10D;&#xE1;st m&scaron;e)</klr></trd>,
    <trd>ofertorium</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Offertorium</trd>,
    <trd>Opfergebet</trd>
  </trdgrp>
  <trd lng="el">
  &pi;&rho;&omikron;&sigma;&kappa;&omikron;&my;&jota;&delta;&eta_ton;
  </trd>
  <trd lng="en">offertory</trd>
  <trd lng="fr">offertoire</trd>
  <trdgrp lng="hu">
    <trd>felaj&aacute;nl&aacute;s</trd>,
    <trd>offert&oacute;rium</trd>
  </trdgrp>
  <trd lng="it">offertorio</trd>
  <trdgrp lng="ja">
    <trd>&#22857;&#x732E;  <pr>&#x307B;&#12358;&#12369;&#12435;</pr></trd>,
    <trd>&#22857;&#x732E;&#x6B4C;  <pr>&#12411;&#12358;&#12369;&#12435;&#12363;</pr></trd>
  </trdgrp>
  <trd lng="nl">offertorium</trd>
  <trdgrp lng="pl">
    <trd>ofiarowanie</trd>,
    <trd>ofertorium</trd>
  </trdgrp>
  <trd lng="pt">ofert&oacute;rio</trd>
  <trd lng="ru">
  &c_p;&c_r;&c_o;&c_s;&c_k;&c_o;&c_m;&c_i;&c_d;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>obetovanie</trd>,
    <trd>ofert&#xF3;rium</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&#x43E;&c_s;&#x43A;&#x43E;&#x43C;&c_i;&c_d;&c_ib;&#x44F;</trd>,
    <trd>&#x43F;&c_r;&c_i;&#x43D;&#x43E;&c_sh;&c_je;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
