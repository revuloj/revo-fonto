<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>fu</rad> <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="fu.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="fu.0.SON">
    <dif>
      <ref tip="lst" cel="son.0o" lst="voo:sonoj">Sono</ref> 
      de <ref tip="super" cel="spir.el0i">elspiro</ref> 
      pro facili&gcirc;o, malstre&ccirc;i&gcirc;o:
      <ekz>
        <tld/>! &ndash; &scirc;i diris, sidi&gcirc;ante kaj malfacile spirante, 
        kiel mi laci&gcirc;is!
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen 
        titolo</vrk>, <lok>&ccirc;apitro 10a</lok></fnt>;
      </ekz>
      <ekz>
        senpezigite, la viro elspiras: &leftquot;Haaaa, <tld/>!&rightquot;
        <fnt>
        <aut>Rikardo Cash</aut>,
        <vrk>Voja&gcirc;o en dezerton</vrk>,
        <lok><url
        ref="http://www.eventoj.hu/arkivo/eve-192.htm">Eventoj, 2000-09,
         numero 192a</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">uf!</trd>
  <trd lng="fr">ouf</trd>
  <trd lng="pt">uf</trd>
</drv>

<drv mrk="fu.00">
  <kap><tld/> <tld/>...</kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="fu.00.SON">
    <dif>
      <ref tip="lst" cel="son.0o" lst="voko.sonoj">Sono</ref> 
      de detenata <ref tip="super" cel="rid.0i">ridado</ref>:
      <ekz>
        maljunulino-oficiredzino entrafas, iafoje &ccirc;ambristino 
        enmontri&gcirc;as tia... <tld/>, <tld/>, <tld/>!
        <fnt><bib>Revizoro</bib>, <lok>akto 2a, sceno 1a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">hi hi hi!</trd>
  <trd lng="fr">hou hou<klr> (rire &eacute;touff&eacute;)</klr></trd>
  <trd lng="pt">hi hi <klr tip="amb">(riso contido)</klr></trd>
</drv>
</art>

