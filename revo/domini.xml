<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: domini.xml,v 1.23 2022/10/30 16:41:47 revo Exp $">
<kap>
  <rad>domini</rad>/o <fnt><bib>SPV</bib></fnt>,
  <var><kap>dominion/o <fnt><vrk>Pekoteko</vrk> <lok>1992</lok></fnt></kap></var>
</kap>
<drv mrk="domini.0o">
  <kap><tld/>o,
  <var><kap>dominiono</kap></var></kap>
  <snc mrk="domini.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Eks<ref tip="vid" cel="koloni.0o.lando">kolonio</ref> de Britujo havanta
      propran registaron, agnoskanta almena&ubreve; formale la anta&ubreve;e
      regantan regnon: 
      <ekz>
        niaj <tld/>oj kaj kolonioj
        <fnt>
        <bib>Salamandroj</bib>,
        <lok>libro 3a, &ccirc;apitro 4a</lok>
        </fnt>;
      </ekz>
      <ekz>
        se mi diras: &leftquot;la <tld/>oj, Kanado, A&ubreve;stralio,
        Nov-Zelando, Sudafriko!&rightquot;, ili dra&scirc;os min
        <fnt>
        <bib>Mortul&Scirc;ip</bib>
        <lok>&ccirc;apitro 9a</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni ne estas solaj <klr>[&#8230;]</klr> pli kaj pli venos, anka&ubreve; de 
        niaj dominionoj
        <fnt>
          <aut>J. Francis</aut>
          <vrk>La Granda Kaldrono</vrk>
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_m;&c_ib;&c_n;&c_ib;&c_jo;&c_n;</trd>
  <trd lng="de">Dominion</trd>
  <trd lng="en">dominion</trd>
  <trdgrp lng="cs">
    <trd>dominion</trd>,
    <trd>dominium</trd>
  </trdgrp>
  <trd lng="es">dominio</trd>
  <trd lng="fr">dominion</trd>
  <trd lng="he">&dalet;&vav;&mem;&yod;&nun;&yod;&vav;&fnun; (&mem;&dalet;&yod;&nun;&he; &ayin;&tsadi;&mem;&alef;&yod;&tav; &bet;&bet;&resh;&yod;&tet;&nun;&yod;&he;)</trd>
  <trd lng="pl">dominium</trd>
  <trd lng="pt">dom&iacute;nio</trd>
  <trd lng="sk">dom&iacute;nium</trd>
  <trd lng="uk">&c_d;&c_o;&c_m;&c_ib;&c_n;&c_ib;&c_o;&c_n;</trd>
</drv>
</art>
