<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <rad>prompt</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="prompt.0a">
   <kap><tld/>a</kap>
   <uzo tip="stl">RAR</uzo>
   <snc>
     <dif>
       Tuj, rapide kaj efike aganta.
     </dif>
   </snc>
  <trdgrp lng="be">
     <trd>&c_h;&c_u;&c_t;&c_k;&c_ib;</trd>,
     <trd>&c_n;&c_je;&c_a;&c_d;&c_k;&c_l;&c_a;&c_d;&c_n;&c_y;</trd>
   </trdgrp>
  <trd lng="ca">prompte<klr> (adj.)</klr></trd>
  <trd lng="fr">prompt</trd>
  <trd lng="hu">azonnali</trd>
  <trd lng="nl">prompt<klr tip="ind"> (bn.)</klr></trd>
  <trdgrp lng="pl">
     <trd>pr&eogonek;dki</trd>,
     <trd>raptowny</trd>
   </trdgrp>
  <trdgrp lng="pt">
     <trd>pronto</trd>,
     <trd>prestes a</trd>
   </trdgrp>
  <trd lng="ro">prompt</trd>
  <trdgrp lng="ru">
     <trd>&c_b;&c_y;&c_s;&c_t;&c_r;&c_y;&c_j;</trd>,
     
<trd>&c_n;&c_je;&c_z;&c_a;&c_m;&c_je;&c_d;&c_l;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>pohotový</trd>,
    <trd>promptný</trd>
  </trdgrp>
</drv>

<drv mrk="prompt.0e">
   <kap><tld/>e</kap>
   <uzo tip="stl">RAR</uzo>
   <snc>
     <dif>
       Senprokraste, rapide decidante a&ubreve; plenumante:
       <ekz>
         Jado <tld/>e sin levis kaj hastis al la portalo de la domo
         <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 64a, p. 
390a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
  <trdgrp lng="be">
     <trd>&c_h;&c_u;&c_t;&c_k;&c_a;</trd>,
     <trd>&c_n;&c_je;&c_a;&c_d;&c_k;&c_l;&c_a;&c_d;&c_n;&c_a;</trd>
   </trdgrp>
  <trd lng="ca">promptament</trd>
  <trd lng="fr">promptement</trd>
  <trd lng="hu">azonnal</trd>
  <trd lng="nl">prompt<klr tip="ind"> (bw.)</klr></trd>
  <trdgrp lng="pl">
     <trd>nagle</trd>,
     <trd>raptownie</trd>
   </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_b;&c_y;&c_s;&c_t;&c_r;&c_o;</trd>,
     
<trd>&c_n;&c_je;&c_z;&c_a;&c_m;&c_je;&c_d;&c_l;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;</trd>
   </trdgrp>
  <trd lng="sk">promptne</trd>
</drv>
</art>
