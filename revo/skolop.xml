<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>skolop</rad>/o
</kap>
<drv mrk="skolop.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="skolop.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Scolopax</trd>).
    </dif>
  </snc>
  <trd lng="cs">sluka lesni</trd>
  <trd lng="de">Waldschnepfe</trd>
  <trd lng="en">woodcock</trd>
  <trd lng="hu">erdeiszalonka</trd>
  <trd lng="pl">s&lstroke;onka</trd>
  <trd lng="ru">&c_v;&c_a;&c_l;&c_mol;&c_d;&c_sh;&c_n;&c_je;&c_p;</trd>
</drv>
<drv mrk="skolop.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" 
      cel="hxarad.0oformaj">&hcirc;aradrioformaj birdoj</ref>
      (<trd lng="la">Scolopacidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Schnepfen <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>
