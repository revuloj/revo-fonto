<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: estrad.xml,v 1.21 2021/05/24 13:22:21 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>estrad</rad>/o
</kap>
<drv mrk="estrad.0o">
  <kap><tld/>o</kap>
  <snc mrk="estrad.0o.podio">
    <dif>
      <ref tip="dif" cel="podi.0o">Podio</ref>:
      <ekz>
        en la fundo estis alta <tld/>o, sur kiu sub ri&ccirc;a tendo staris malgranda tablo
        kaj kanapo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        en Berno, je 1913, <nom>Zamenhof</nom> jam ne parolis, e&ccirc; ne sidis sur <tld/>o
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        li teatre kisis sian Majstron sur <tld/>o de l' &ccirc;ambrego
        <fnt>
          <bib>VivZam</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="tribun.0o">tribuno</ref>
    <refgrp tip="vid">
      <ref cel="peron.0o.platajxo">perono<sncref/></ref>,
      <ref cel="piedes.0o">piedestalo</ref>,
      <ref cel="plac.0eto.speciala">placeto<sncref/></ref>,
      <ref cel="postam.0o">postamento</ref>,
      <ref cel="scen.0ejo">scenejo</ref>,
      <ref cel="teras.0o">teraso</ref>
    </refgrp>
  </snc>
  <ref tip="hom" cel="estr.0ado">estr&middot;ad&middot;o</ref>
  <trdgrp lng="be">
    <trd>&c_e;&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_m;&c_o;&c_s;&c_t;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_je;&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_i;&c_u;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>estr&aacute;da</trd>,
    <trd>p&oacute;dium</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Estrade</trd>,
     <trd>Podest</trd>,
     <trd>Podium</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>platform</trd>,
     <trd>stage</trd>
  </trdgrp>
  <trd lng="es">estrado</trd>
  <trd lng="fr">estrade</trd>
  <trd lng="he">&dalet;&vav;&#1468;&kaf;&#1464;&fnun;</trd>
  <trdgrp lng="hu">
     <trd>esztr&aacute;d</trd>,
     <trd>p&oacute;dium</trd>,
     <trd>dobog&oacute;</trd>,
     <trd>emelv&eacute;ny</trd>
  </trdgrp>
  <trd lng="nl">estrade</trd>
  <trd lng="pl">estrada</trd>
  <trd lng="pt">p&oacute;dio</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_m;&c_o;&c_s;&c_t;&c_k;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vedenie</trd>,
    <trd>velenie</trd>
  </trdgrp>
</drv>
</art>
