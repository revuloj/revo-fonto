<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lutreol.xml,v 1.7 2024/03/11 09:22:20 revo Exp $">
<kap>
  <rad>lutreol</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="lutreol.0o">
  <kap><tld/>o</kap>
  <snc mrk="lutreol.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Malgranda <ref tip="lst" cel="best.rabo0o" lst="voko:rabobestoj">rabobesto</ref>
      vivanta apud akvejoj en E&ubreve;ropo;
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:zoologiaj_specioj">specio</ref> de <ref
      tip="super" cel="mustel.0o">mustelo</ref>
      (<trd lng="la">Mustela lutreola</trd>) kun ru&gcirc;ete &gcirc;is nigre bruna felo,
      mallonga kruroj kaj vosto, blanka makulo sub la muzelo:
      <ekz>
        diversaj rabobestoj, interalie <tld/>oj, lernis eltrovadi kaj
        rabi la nestojn de mevoj
        <fnt>
          <aut>A. Bach</aut>,
          <vrk><url ref="http://esperantaretradio.blogspot.co.at/2014/01/argentmevo.html">
          Ar&gcirc;entmevo</url></vrk>,
          <lok>Esperanta Retradio, 2014-01-14</lok>
        </fnt>;
      </ekz>
      <ekz>
        inter prionaj malsanoj estas <klr>[&#8230;]</klr> transdonebla encefalopatio de
        <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2006/008861.php"
              >Nova etapo en biologio kaj medicino</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/d/d5/European_Mink_at_Osnabr&uuml;ck_Zoo_02_cropped.jpg/640px-European_Mink_at_Osnabr&uuml;ck_Zoo_02_cropped.jpg"
      prm="CC BY 2.0"
      ><tld/>o en besto&gcirc;ardeno Osnabr&uuml;ck
        <fnt>
          <aut>zoofanatic - adaptita de Abujoy</aut>
          <lok><url ref="&WCW;/File:European_Mink_at_Osnabr%C3%BCck_Zoo_02_cropped.jpg"
              >https://commons.wikimedia.org/wiki/File:European_Mink_at_Osnabr&uuml;ck_Zoo_02_cropped.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_w;&c_r;&c_a;&c_p;&c_je;&c_j;&c_s;&c_k;&c_a;&c_ja; <ind>&c_n;&c_o;&c_r;&c_k;&c_a;</ind></trd>,
    <trd>&c_n;&c_o;&c_r;&c_y;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">norek</trd>
  <trd lng="de">Nerz</trd>
  <trd lng="en">european mink</trd>
  <trd lng="fr">vison d'Europe</trd>
  <trd lng="pl">norka europejska</trd>
  <trd lng="sk">norka</trd>
  <trd lng="uk">&c_n;&c_o;&c_r;&c_k;&c_a;</trd>
</drv>
</art>

