<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>lutreol</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="lutreol.0o">
  <kap><tld/>o</kap>
  <snc mrk="lutreol.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Malgranda <ref tip="lst"
      cel="best.rabo0o" lst="voko:rabobestoj">rabobesto</ref>
      vivanta apud akvejoj en E&ubreve;ropo;
      <ref tip="lst" cel="speci.0o.BIO"
      lst="voko:zoologiaj_specioj">specio</ref> de <ref tip="super"
      cel="mustel.0o">mustelo</ref>,
      (<trd lng="la">Mustela lutreola</trd>):
      <ekz>
        diversaj rabobestoj, interalie <tld/>oj, lernis eltrovadi kaj 
        rabi la nestojn de mevoj
        <fnt>
          <aut>A. Bach</aut>,
          <vrk><url
          ref="http://esperantaretradio.blogspot.co.at/2014/01/argentmevo.html">
          Ar&gcirc;entmevo</url></vrk>,
          <lok>Esperanta Retradio, 2014-01-14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">european mink</trd>
  <trd lng="fr">vison d'Europe</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>