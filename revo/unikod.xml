<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Unikod</rad>/o
  <fnt><lok><url ref="&KompLeks;/U.html#UNIKODO">KompLeks</url></lok></fnt>
  <fnt><aut>Bertilo Wennergren</aut>: <lok><url
  ref="http://purl.oclc.org/net/bertilow?d=html/signokodoj/unikodo.htm">
  Unikodo</url></lok></fnt>
</kap>
<drv mrk="unikod.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KOMP</uzo>
  <snc>
    <dif>
      Internacia norma <ref tip="super" cel="kod.0o.SCI">kodo</ref>
      kies signaro ebligas prezenti plurlingvajn tekstojn
      entenantajn signojn de diversaj alfabetoj (inklude la &ccirc;inan
      kaj Esperantan):
      <ekz>
        nur eta malplimulto de la hodia&ubreve;aj retuzantoj povas &gcirc;uste trakti
        mesa&gcirc;ojn en <tld/>o<fnt><aut>Edmund Grimley Evans</aut>:
        <vrk>Uniksulo ekspertas Unikodon</vrk>, <bib>Monato</bib><lok>jaro
        2000a, numero 2a, p. 20a</lok></fnt>.
      </ekz>
    </dif>
    <rim>
      <ref cel="aski.0o">Askio</ref> estas subkodo de <tld/>o.
    </rim>
  </snc>
  <trd lng="be">&c_U;&c_n;&c_ib;&c_k;&c_o;&c_d;</trd>
  <trdgrp lng="de">
    <trd>Unicode</trd>,
    <trd>Unikode</trd>
  </trdgrp>
  <trd lng="ca">Unicode</trd>
  <trd lng="en">Unicode</trd>
  <trd lng="es">Unicode</trd>
  <trdgrp lng="fr">
    <trd>Unicode</trd>
  </trdgrp>
  <trd lng="he">&yod;&vav;&nun;&yod;&qof;&vav;&dalet;</trd>
  <trd lng="hu">Unicode</trd>
  <trd lng="nl">Unicode</trd>
  <trdgrp lng="ru">
    <trd>&c_U;&c_n;&c_i;&c_k;&c_o;&c_d;</trd>, <trd>&c_Ju;&c_n;&c_i;&c_k;&c_o;&c_d;</trd>
  </trdgrp>
</drv>
</art>
