<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap><rad>Unikod</rad>/o
        <fnt><lok><url ref="&KompLeks;/U.html#UNIKODO">KompLeks</url></lok></fnt>
        <fnt><aut>Bertilo Wennergren</aut>: <lok><url
            ref="http://purl.oclc.org/net/bertilow?d=html/signokodoj/unikodo.htm"
            >Unikodo</url></lok></fnt></kap>
    <drv mrk="unikod.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">KOMP</uzo>
      <snc>
        <dif>
          Internacia norma <ref tip="super" cel="kod.0o.SCI">kodo</ref>
          kies signaro ebligas prezenti plurlingvajn tekstojn
          entenantajn signojn de diversaj alfabetoj (inklude la &ccirc;inan
          kaj Esperantan).
        </dif>
        <rim>
          <ref tip="sub" cel="aski.0o">Askio</ref> estas subkodo de <tld/>o.
        </rim>
      </snc>
      <trd lng="en">Unicode</trd>
      <trdgrp lng="ru"><trd>&c_U;&c_n;&c_i;&c_k;&c_o;&c_d;</trd>, <trd>&c_Ju;&c_n;&c_i;&c_k;&c_o;&c_d;</trd></trdgrp>
    </drv>
  </art>

