<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: unikod.xml,v 1.23 2024/02/20 01:21:40 revo Exp $">
<kap>
   <rad>Unikod</rad>/o
   <fnt><lok><url ref="&amp;KompLeks/U.html#UNIKODO">KompLeks</url></lok></fnt>
   <fnt><aut>Bertilo Wennergren</aut>: <lok><url
   ref="http://purl.oclc.org/net/bertilow?d=html/signokodoj/unikodo.htm">
   Unikodo</url></lok></fnt>
</kap>
<drv mrk="unikod.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">KOMP</uzo>
   <snc>
     <dif>
       Internacia norma <ref tip="super" cel="kod.0o.SCI">kodo</ref>
       kies signaro ebligas prezenti plurlingvajn tekstojn
       entenantajn signojn de diversaj alfabetoj (inklude la &ccirc;inan
       kaj Esperantan):
       <ekz>
         nur eta malplimulto de la hodia&ubreve;aj retuzantoj povas
&gcirc;uste trakti
         mesa&gcirc;ojn en <tld/>o<fnt><aut>Edmund Grimley Evans</aut>:
         <vrk>Uniksulo ekspertas Unikodon</vrk>,
<bib>Monato</bib><lok>jaro
         2000a, numero 2a, p. 20a</lok></fnt>.
       </ekz>
     </dif>
     <rim>
       <ref cel="aski.0o">Askio</ref> estas subkodo de <tld/>o.
     </rim>
   </snc>
  <trd lng="be">&c_U;&c_n;&c_ib;&c_k;&c_o;&c_d;</trd>
  <trd lng="ca">Unicode</trd>
  <trd lng="cs">Unikodo</trd>
  <trdgrp lng="de">
     <trd>Unicode</trd>,
     <trd>Unikode</trd>
   </trdgrp>
  <trd lng="en">Unicode</trd>
  <trd lng="es">Unicode</trd>
  <trd lng="fa">&f_y;&a_w;&a_n;&f_y;&#8204;&f_k;&a_damma;&a_d;</trd>
  <trd lng="fi">Unicode</trd>
  <trdgrp lng="fr">
     <trd>Unicode</trd>
   </trdgrp>
  <trd lng="he">&yod;&vav;&nun;&yod;&qof;&vav;&dalet;</trd>
  <trd lng="hu">Unicode</trd>
  <trd lng="it">Unicode</trd>
  <trdgrp lng="ja">
    <trd>&#x30E6;&#x30CB;&#x30B3;&#x30FC;&#x30C9;</trd>,
    <trd>Unicode</trd>
  </trdgrp>
  <trd lng="nl">Unicode</trd>
  <trd lng="pl">system znak&oacute;w<ind>  Unicode</ind></trd>
  <trdgrp lng="pt">
    <trd>Unicode</trd>
   </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_U;&c_n;&c_i;&c_k;&c_o;&c_d;</trd>,
<trd>&c_Ju;&c_n;&c_i;&c_k;&c_o;&c_d;</trd>
   </trdgrp>
  <trd lng="sk">Unik&#xF3;d</trd>
  <trd lng="sv">Unicode</trd>
  <trdgrp lng="uk">
    <trd>&#x42E;&#x43D;&c_ib;&#x43A;&#x43E;&c_d;</trd>,
    <trd>&c_U;&#x43D;&c_ib;&#x43A;&#x43E;&c_d;</trd>
  </trdgrp>
</drv>
</art>
