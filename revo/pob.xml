<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pob.xml,v 1.14 2024/02/17 07:52:34 revo Exp $">
<kap>
  <rad>pob</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="pob.0o">
  <kap><tld/>o</kap>
  <snc mrk="pob.0o.MAR">
    <uzo tip="fak">MAR</uzo>
        <dif>
          La malanta&ubreve;a parto de
          <ref tip="malprt" cel="sxip.0o.MAR">&scirc;ipo</ref>,
          boato ks, kontraste al la
          <ref tip="ant" cel="pru.0o">pruo</ref>:
          <ekz>
            bril-koloraj flagetoj flirtis de pruo al <tld/>o kaj dol&ccirc;e
            bonodoraj floroj &scirc;uti&gcirc;is sur la tuta ferdeko 
            <fnt>
              <aut>W. B. Gant, trad. N. Gant</aut>,
              <vrk>Eks-Sklavo...</vrk>,
              <lok><url ref="http://www.medialabinc.net/temp/fasko/nask/9x.htm">La NASKa Fasko,
              2001, numero 9a</url></lok>
           </fnt>;
          </ekz><ekz>
            la boatisto &ccirc;e la <tld/>o premis remilon al la bordo 
            <fnt>
              <aut>Yi Ye</aut>,
              <vrk><url ref="http://english.hanban.edu.cn/world/shi-lvyou/itinero_proponita/29.htm">Voja&gcirc;o sur
              Mengdong-rivero</url></vrk>,
              <lok>2004-02</lok>
           </fnt>.
          </ekz>
        </dif>
        <ref tip="sin" cel="pouxp.0o.MAR">po&ubreve;po</ref>
      </snc>
      <trd lng="be">&c_k;&c_a;&c_r;&c_m;&c_a;&#x301; <klr>(&c_k;&c_a;&c_r;&c_a;&c_b;&c_l;&c_ja;)</klr></trd>
  <trd lng="ca">popa</trd>
  <trdgrp lng="cs">
    <trd>lodn&iacute; z&aacute;&dcaron;</trd>,
    <trd>zadn&iacute; paluba</trd>
  </trdgrp>
  <trdgrp lng="en">
        <trd>stern</trd>,
        <trd>poop</trd>
      </trdgrp>
  <trd lng="fr">poupe</trd>
  <trd lng="nl">achtersteven</trd>
  <trd lng="pl">rufa</trd>
  <trd lng="ro">pupa</trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_m;&c_a;</trd>
  <trd lng="sk">korma <klr>(zad lode)</klr></trd>
  <trdgrp lng="zh">
    <trd>&#x8244; <pr>sh&#x101;o</pr></trd>,
    <trd>&#x8239;&#x8244; <pr>chu&#xE1;nsh&#x101;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pob.P0o">
  <kap><tld lit="P"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
  </kap>
  <mlg>Pup</mlg>
  <snc mrk="pob.P0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Sud-hemisfera <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref>
      (<trd lng="la">Puppis</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_K;&c_a;&c_r;&c_m;&c_a;&#x301; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#x2019;&c_je;)</klr></trd>
  <trd lng="en">Puppis</trd>
  <trd lng="fr">Poupe<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="pl">Rufa  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Pupa  <klr>(constela&#x21B;ie)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&c_r;&#x43C;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_d;&#x43D;&#x44F; &c_v;&c_je;&c_r;&c_h;&#x43D;&#x44F; &#x43F;&c_a;&#x43B;&c_u;&c_b;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#x8239;&#x5C3E;&#x5EA7; <pr>Chu&#xE1;nw&#x11B;izu&#xF2;</pr></trd>
</drv>
</art>

