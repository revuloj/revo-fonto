<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>pob</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="pob.0o">
  <kap><tld/>o</kap>
  <snc mrk="pob.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <ref tip="dif" cel="pouxp.0o.MAR">po&ubreve;po</ref>
    <ref tip="ant" cel="pru.0o">pruo</ref>
      <ekz>
        bril-koloraj flagetoj flirtis de pruo al <tld/>o kaj dol&ccirc;e
        bonodoraj floroj &scirc;uti&gcirc;is sur la tuta ferdeko 
        <fnt>
          <aut>W. B. Gant, trad. N. Gant</aut>,
          <vrk>Eks-Sklavo...</vrk>,
          <lok><url
          ref="http://www.medialabinc.net/temp/fasko/nask/9x.htm">
          La NASKa Fasko, 2001, numero 9a</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la boatisto &ccirc;e la <tld/>o premis remilon al la bordo 
        <fnt>
          <aut>Yi Ye</aut>,
          <vrk><url ref=
"http://english.hanban.edu.cn/world/shi-lvyou/itinero_proponita/29.htm">
          Voja&gcirc;o sur Mengdong-rivero</url></vrk>,
          <lok>2004-02</lok>
        </fnt>.
      </ekz>
      </snc>
  <trd lng="ca">popa</trd>
  <trdgrp lng="cs">
    <trd>lodní záď</trd>,
    <trd>zadní paluba</trd>
  </trdgrp>
  <trdgrp lng="en">
        <trd>stern</trd>,
        <trd>poop</trd>
      </trdgrp>
  <trd lng="fr">poupe</trd>
  <trd lng="nl">achtersteven</trd>
  <trd lng="pl">rufa</trd>
  <trd lng="sk">korma <klr>(zad lode)</klr></trd>
</drv>
</art>

