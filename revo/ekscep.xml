<kap>
  <rad>ekscepci</rad>/o
</kap>
<drv mrk="ekscep.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <snc mrk="ekscep.0o.JUR">
    <dif>
      Defenda rimedo por prokrasti la finon de la proceso a&ubreve; por
      kritiki &gcirc;ian formon:
      <ekz>
        la alianco esprimas fortan malkontenton <klr>[&#x2026;]</klr> al la Konsilio de EU, kiu
        ignorante la faktojn de enketado kaj <tld/>o, malobservas la kontra&ubreve;dumpingan
        regularon de EU
        <fnt>
          <vrk><url ref="&CRI;1/2006/07/19/1@42044.htm">&Ccirc;ina ekonomio kreskis je 10,9% en unua duono de kuranta jaro</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x5F02;&#x8BAE; <pr>y&#xEC;y&#xEC;</pr></trd>
</snc>
  <rim>
    Ne konfuzu kun <ref cel="escept.0o">escepto</ref>.
  </rim>
  <trd lng="be">&c_p;&c_ja;&c_r;&c_e;&c_ch;&c_a;&c_n;&c_mol;&c_n;&c_je;
    <klr tip="amb">(&c_u; &c_s;&c_u;&c_d;&c_z;&c_je;)</klr></trd>
  <trdgrp lng="cs">
    <trd>excepce</trd>,
    <trd>n&aacute;mitka <klr>(procesn&iacute;)</klr></trd>,
    <trd>odpor (procesn&iacute; obrana)</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Vorbehalt</trd>,
    <trd>Einwand</trd>,
    <trd>Einrede</trd>,
    <trd>Einspruch wegen Formfehler</trd>
  </trdgrp>
  <trd lng="fr">exception<klr tip="ind"> (droit)</klr></trd>
  <trdgrp lng="hu">
       <trd>kifog&aacute;s</trd>,
       <trd>ellentmond&aacute;s</trd>,
       <trd>&oacute;v&aacute;s</trd>
    </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6297;&#x5F01;  <pr>&#x3053;&#x3046;&#x3079;&#x3093;</pr></trd>,
    <trd>&#x7570;&#x8B70;&#x7533;&#x7ACB;  <pr>&#x3044;&#x304E;&#x3082;&#x3046;&#x3057;&#x305F;&#x3066;</pr></trd>
  </trdgrp>
  <trd lng="pl">wy&lstroke;&aogonek;czenie</trd>
  <trd lng="pt">exce&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_o;&c_t;&c_v;&c_o;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>excepcia</trd>,
    <trd>n&aacute;mietka</trd>,
    <trd>obrana</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x456;&#x434;&#x432;&#x456;&#x434;</trd>,
    <trd>&#x437;&#x430;&#x43F;&#x435;&#x440;&#x435;&#x447;&#x435;&#x43D;&#x43D;&#x44F; &#x441;&#x442;&#x43E;&#x440;&#x43E;&#x43D;&#x438;</trd>
  </trdgrp>
</drv>
