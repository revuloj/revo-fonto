<art mrk="$Id: aga.xml,v 1.4 2022/05/13 07:32:15 revo Exp $">
<kap>
  <rad>aga</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="aga.0o">
  <kap><tld/>o</kap>
  <snc mrk="aga.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Osmanid-devena honora <ref tip="super" cel="titol.0o.RANG">titolo</ref> de militaj a&ubreve;
      civilaj estroj:
      <ekz>
        la &ccirc;evalo de la <tld/>o forkuris
        <fnt>
          <aut>G. G&aacute;rdonyi, trad. J. Horvath</aut>
          <vrk><url ref="http://vortaro.hu/stelspec.pdf">
          Steloj de Eger</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="sinjor.0o.mastro">sinjoro<sncref/></ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_g;&c_a; <klr>(&c_a;&c_s;&c_m;&c_a;&c_n;&c_s;&c_k;&c_ib; &c_t;&c_y;&c_t;&c_u;&c_l;)</klr></trd>,
    <trd>&c_a;&c_gu;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Agha</trd>
  <trd lng="fr">aga</trd>
  <trd lng="uk">&c_a;&c_gu;&c_a; <klr>(&c_t;&c_u;&c_r;&c_je;&c_c;&c_mol;&c_k;&c_i;&c_j; &c_o;&c_f;&c_ib;&c_c;&c_je;&c_r;)</klr></trd>
</drv>
</art>

