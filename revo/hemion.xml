<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hemion.xml,v 1.17 2023/08/31 15:46:16 revo Exp $">
<kap>
  <rad>hemion</rad>/o
</kap>
<drv mrk="hemion.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_specioj" cel="speci.0o.BIO">Specio</ref>
      de <ref tip="super" cel="cxeval.0o.genro">&ccirc;evalo</ref>
      (<trd lng="la">Equus hemionus</trd>), vivanta en centra kaj
      okcidenta Azio kaj iom simila al <ref tip="vid" cel="azen.0o">azeno</ref>:
      <ekz>
        la sova&gcirc;a &ccirc;evalo la&ubreve; ekstera aspekto diferencigeblas de la <tld/>o (duonazeno)
        kaj la afrika sova&gcirc;a azeno &ccirc;efe per la pli mallongaj oreloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=288221">Sova&gcirc;a &ccirc;evalo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="lst" cel="huf.nepar0ulo" lst="voko:neparhufuloj">neparhufulo</ref>
    <bld lok="&WCU;/thumb/5/56/Equus_hemionus_khur_1.jpg/600px-Equus_hemionus_khur_1.jpg" prm="CC BY-SA 2.0"><tld/>o (Equus hemionus khur), Gujerat, Barato
        <fnt>
          <aut>Lip Kee Yap el Singapuro</aut>
          <lok><url ref="&WCW;/File:Equus_hemionus_khur_1.jpg">&WCW;/File:Equus_hemionus_khur_1.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_l;&c_a;&c_n;</trd>
  <trd lng="de">Halbesel</trd>
  <trd lng="fr">h&eacute;mione</trd>
  <trdgrp lng="hu">
    <trd>kul&aacute;n</trd>,
    <trd>f&eacute;lszam&aacute;r</trd></trdgrp>
   <trdgrp lng="it">
    <trd>onagro</trd>,
    <trd>emione</trd>
    </trdgrp>
  <trd lng="ja">&#x30A2;&#x30B8;&#x30A2;&#x30CE;&#x30ED;&#x30D0;</trd>
  <trd lng="nl">wilde <ind>ezel</ind></trd>
  <trd lng="pt">hem&iacute;ono</trd>
  <trd lng="ru">&c_k;&c_u;&c_l;&c_a;&c_n;</trd>
  <trd lng="uk">&c_k;&c_u;&c_l;&c_a;&c_n;</trd>
</drv>
</art>
