    <trd>kul&aacute;n</trd>,
    <trd>f&eacute;lszam&aacute;r</trd></trdgrp>
  <trd lng="nl">wilde <ind>ezel</ind></trd>
  <trd lng="ru">&c_k;&c_u;&c_l;&c_a;&c_n;</trd>
  <trd lng="uk">&c_k;&c_u;&c_l;&c_a;&c_n;</trd>
</drv>
</art>
