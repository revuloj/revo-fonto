<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ubliet.xml,v 1.7 2022/03/28 11:08:53 revo Exp $">
<kap>
    <rad>ubliet</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ubliet.0o">
    <kap><tld/>o</kap>
    <uzo tip="stl">RAR</uzo>
    <snc mrk="ubliet.0o.POL">
      <dif>
        Subtera, malkomforta, malvasta <ref tip="super" cel="liber.mal0ejo.POL">malliberejo</ref>, kiajn havis antikvaj
        fortika&jcirc;oj:
        <ekz>
           en la plej profundaj kaj tenebraj <tld/>oj de la infero
          <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 11a, p.
          453a</lok></fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="karcer.0o.cxelo">karcero <sncref/></ref>
      </refgrp>
    </snc>
  <trdgrp lng="ca">
      <trd>masmorra</trd>,
      <trd>t&agrave;vega</trd>
    </trdgrp>
  <trd lng="cs">lidomorna</trd>
  <trd lng="fi">kellarityrm&auml;</trd>
  <trd lng="fr">oubliette</trd>
  <trd lng="it">segreta<klr> (sost.)</klr></trd>
  <trd lng="sk">&#x13E;udomor&#x148;a</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x456;&#x434;&#x437;&#x435;&#x43C;&#x43D;&#x430; &#x432;&#x2019;&#x44F;&#x437;&#x43D;&#x438;&#x446;&#x44F;</trd>,
    <trd>&#x43A;&#x430;&#x43C;&#x2019;&#x44F;&#x43D;&#x438;&#x439; &#x43C;&#x456;&#x448;&#x43E;&#x43A;</trd>
  </trdgrp>
</drv>
</art>

