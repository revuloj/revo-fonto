<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ubliet.xml,v 1.11 2024/05/10 12:37:47 revo Exp $">
<kap>
    <rad>ubliet</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ubliet.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="ubliet.0o.HIS">
    <dif>
      <ke>Forgeskelo</ke>, <ke>punkelo</ke>; subtera, malkomforta, malvasta <ref tip="super" cel="liber.mal0ejo.POL">malliberejo</ref>,
      kiajn havis fortika&jcirc;oj:
      <ekz>
         en la plej profundaj kaj tenebraj <tld/>oj de la infero
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 11a, p.
        453a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="karcer.0o.cxelo">karcero<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_ja;&c_m;&c_n;&c_ib;&c_c;&c_a;</trd>,
    <trd>&c_a;&c_s;&c_t;&c_r;&c_o;&c_g;</trd>,
    <trd>&c_ja;&c_m;&c_a; <klr tip="amb">(&c_v;&c_ja;&c_z;&c_mol;&c_n;&c_ib;&c_c;&c_a;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ca">
      <trd>masmorra</trd>,
      <trd>t&agrave;vega</trd>
    </trdgrp>
  <trd lng="cs">lidomorna</trd>
  <trd lng="de">Verlies</trd>
  <trdgrp lng="en">
    <trd>bottle dungeon</trd>,
    <trd>oubliette</trd>
  </trdgrp>
  <trd lng="fi">kellarityrm&auml;</trd>
  <trd lng="fr">oubliette</trd>
  <trd lng="it">segreta<klr> (sost.)</klr></trd>
  <trd lng="pl">loch</trd>
  <trd lng="sk">&lcaron;udomor&ncaron;a</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_ib;&c_d;&c_z;&c_je;&c_m;&c_n;&c_a; &c_v;&#8217;&c_ja;&c_z;&c_n;&c_i;&c_c;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_m;&#8217;&c_ja;&c_n;&c_i;&c_j; &c_m;&c_ib;&c_sh;&c_o;&c_k;</trd>
  </trdgrp>
</drv>
</art>

