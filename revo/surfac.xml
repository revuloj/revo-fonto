<drv mrk="surfac.0a">
  <kap><tld/>a</kap>
  <snc mrk="surfac.0a.ekstera">
    <dif>
      Je la <tld/>o:
      <ekz>
        la &ccirc;ifonoj estu nur supra kaj <tld/>a kamuflo
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kosmo&scirc;ipo kunportos unuhoman motor&ccirc;areton, kiu ebligos <tld/>an esploradon
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2010/009989.php">Voja&gcirc;o unudirekta</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekster.0a">ekstera</ref>
    </refgrp>
  </snc>
  <snc mrk="surfac.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Facile rimarkebla,
      <refgrp tip="sin">
        <ref cel="supr.0a">supra&jcirc;a</ref>,
        <ref cel="profun.mal0a">malprofunda</ref>
      </refgrp>:
      <ekz>
        via opinio pri la okaza&jcirc;oj &scirc;ajnas malkompetenta kaj <tld/>a
        <fnt>
          <bib>Monato</bib>
          <aut>Sergej Derevjanny&hcirc;</aut>
          <vrk><url ref="&Monato;/2004/008512.html">Horora</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        harmonio inter kristana kredo kaj okcidenta civilizo estas nur <tld/>a
        <fnt>
          <bib>Monato</bib>
          <aut>Kimura Goro Christoph</aut>
          <vrk><url ref="&Monato;/2011/010304.php">Utimura Kanzo anta&ubreve; 150 jaroj
          naski&gcirc;is</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        figuro montranta per krampoj la <tld/>an strukturon de la &ccirc;i-anta&ubreve;a frazo
        <fnt>
          <bib>LAE</bib>
          <lok>5. Leksiko Kaj Semantiko</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_v;&c_je;&c_r;&c_h;&c_n;&c_je;&c_v;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>povrchn&iacute;</trd>,
