<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>8</ofc>
<rad>surfac</rad>/o
<fnt>B</fnt>
</kap>
<drv mrk="surfac.0o">
  <kap><tld/>o</kap>
  <snc mrk="surfac.0o.KOMUNE">
    <dif>
      La ekstera parto de iu korpo:
      <ekz>
        la vertikalaj <tld/>oj de muro.
      </ekz>
    </dif>
    <ref tip="vid" cel="supr.0ajxo.KOMUNE">supra&jcirc;o.</ref>
  </snc>
  <snc mrk="surfac.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <dif>
      Dudimensia <ref tip="super" cel="figur.geometria0o">geometria figuro</ref>:
      <ekz>
        <ref cel="eben.0o.elementa">ebeno</ref> estas <tld/>o;
      </ekz>
      <ekz>
        kvadrato estas ebena <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      Atributoj de surfaco:
      <ref cel="are.0o">areo</ref>,
      <ref cel="crikonf.0o">cirkonferenco</ref>,
      <ref cel="perife.0o">periferio</ref>,
      <ref cel="perime.0o">perimetro</ref>,
      <ref cel="rand.0o.figuro">rando</ref>;
      rimarkindaj ecoj :
      <ref cel="rekt.0ara.surfaco">rektara</ref>,
      <ref cel="rivolu.0a.figuro">rivolua</ref>.
    </refgrp>
  </snc>
  <trd lng="en">surface</trd>
  <trd lng="de">Fl&auml;che</trd>
  <trd lng="fr">surface</trd>
  <trd lng="ru">&c_p;&c_o;&c_v;&c_je;&c_r;&c_h;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>
</art>
