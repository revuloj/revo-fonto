<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>8</ofc>
<rad>surfac</rad>/o
<fnt>B</fnt>
</kap>
<drv mrk="surfac.0o">
  <kap><tld/>o</kap>
  <snc mrk="surfac.0o.KOMUNE">
    <dif>
      La ekstera parto de iu korpo:
      <ekz>
        la vertikalaj <tld/>oj de muro.
      </ekz>
    </dif>
    <ref tip="vid" cel="supr.0ajxo.KOMUNE">supra&jcirc;o.</ref>
  <trd lng="de">Oberfl&auml;che</trd>
  <trd lng="nl">oppervlak</trd>
  </snc>
  <snc mrk="surfac.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 25</lok></fnt>
    <dif>
      Dudimensia
      <ref tip="super" cel="figur.geometria0o">geometria figuro</ref>:
      <ekz>
         ebena <tld/>o <klr>(inkluzivata en ebeno)</klr>;
      </ekz>
      <ekz>
        neebena <tld/>o. 
      </ekz>
    </dif>
    <refgrp tip="sin">
      Malnovaj ekvivalentoj:
      <ref cel="are.0ajxo">area&jcirc;o</ref>,
      <ref cel="supr.0ajxo.MAT">supra&jcirc;o</ref>;
    </refgrp>
    <refgrp tip="sub">
      specifaj ebenaj surfacoj:

      <ref cel="angul.0o.figuro">angulo</ref>,
      <ref cel="cirkl.0o.surfaco">cirklo</ref>,
      <ref cel="eben.0o.elementa">ebeno</ref>,
      <ref cel="later.plur0o">plurlatero</ref>;

      specifaj neebenaj surfacoj:
      <ref cel="cilind.0o.surfaco">cilindro</ref>,
      <ref cel="elipso.0o">elipsoido</ref>,
      <ref cel="konoid.0o">konoido</ref>,
      <ref cel="konus.0o.surfaco">konuso</ref>,
      <ref cel="kvadri1.0o">kvadriko</ref>,
      <ref cel="sfer.0o.surfaco">sfero</ref>,
      <ref cel="hiperb1.0o">hiperboloido</ref>,
      <ref cel="parabo1.0o">paraboloido</ref>,
      rando de <ref cel="edr.plur0o">pluredro</ref>,
      <ref cel="tor.0o.MAT">toro</ref>;
    </refgrp>
    <refgrp tip="vid">
      atributoj de barita surfaco:
      <ref cel="are.0o.MAT">areo</ref>,
      <ref cel="cirkon.0o">cirkonferenco</ref>,
      <ref cel="perife.0o">periferio</ref>,
      <ref cel="perime.0o">perimetro</ref>,
      <ref cel="rand.0o.figuro">rando</ref>;
      
      rimarkindaj ecoj :
      <ref cel="rekt.0ara.surfaco">rektara</ref>,
      <ref cel="rivolu.0a.figuro">rivolua</ref>.
    </refgrp>
    <trd lng="de">Fl&auml;che</trd>
    <trd lng="nl">vlak</trd>
  </snc>
  <trd lng="en">surface</trd>
  <trd lng="fr">surface</trd>
  <trd lng="ru">&c_p;&c_o;&c_v;&c_je;&c_r;&c_h;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sv">yta</trd>
</drv>
</art>
