<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: juglan.xml,v 1.42 2024/03/14 18:27:02 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>jugland</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="juglan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      La <ref tip="super" cel="nuks.0o">nukso</ref> de <tld/>ujo
      (anta&ubreve;e konsiderita kerno de drupo), kun oleri&ccirc;a
      cerbetforma interno:
      <ekz>
        <klr>[&ccirc;e la abio]</klr> oritaj pomoj kaj <tld/>oj pendis kvaza&ubreve; alkreskintaj
        <fnt>
          <bib>Fab2</bib>
          <lok>Abio</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i frotis Elizon per suko de <tld/>o, tiel ke &scirc;i fari&gcirc;is tute mallume bruna
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        belegan lakitan &scirc;elon de <tld/>o &scirc;i ricevis kiel lulilon, bluaj folietoj de
        violo estis &scirc;iaj matracoj, kaj folieto de rozo estis &scirc;ia litkovrilo
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="&WCU;/thumb/0/0c/W_Nuss_Gr_99.jpg/320px-W_Nuss_Gr_99.jpg" prm="CC-BY-SA-3.0">
    <tld/>o kaj &gcirc;ia interno
       <fnt>
         <aut>Horst Frank</aut>
         <lok><url ref="&WCW;/File:W_Nuss_Gr_99.jpg">&WCW;/File:W_Nuss_Gr_99.jpg</url></lok>
       </fnt>
  </bld>
  <trd lng="be">&c_v;&c_a;&c_l;&c_o;&c_s;&c_k;&c_ib;
&c_a;&c_r;&c_e;&c_h;</trd>
  <trd lng="bg">&c_o;&c_r;&c_je;&c_h;</trd>
  <trd lng="br">krao&ntilde;enn<klr tip="ind"> (frouezhenn)</klr></trd>
  <trd lng="ca">nou</trd>
  <trd lng="cs">vla&scaron;sk&yacute; o&rcaron;ech</trd>
  <trd lng="de">Walnuss</trd>
  <trd lng="en">walnut</trd>
  <trd lng="es">nuez <klr>de nogal</klr></trd>
  <trd lng="fr">noix</trd>
  <trd lng="hu">di&oacute;</trd>
  <trd lng="it">noce<klr> (frutto)</klr></trd>
  <trd lng="ja">&#12463;&#12523;&#12511;</trd>
  <trdgrp lng="nl">
   <trd>walnoot</trd>,
   <trd>okkernoot</trd>
  </trdgrp>
  <trd lng="pl">orzech w&lstroke;oski</trd>
  <trd lng="pt">noz</trd>
  <trd lng="ru">&c_g;&c_r;&c_je;&c_c;&c_k;&c_i;&c_j;
&c_o;&c_r;&c_je;&c_h;</trd>
  <trd lng="sk">vla&scaron;sk&yacute; orech</trd>
  <trd lng="sv">valn&ouml;t</trd>
  <trd lng="tr">ceviz</trd>
  <trd lng="uk"><klr>(&c_v;&c_o;&c_l;&c_o;&c_s;&c_mol;&c_k;&c_i;&c_j;)</klr> &c_g;&c_o;&c_r;&c_ib;&c_h;</trd>
  <trdgrp lng="zh">
    <trd>&#26680;&#26691; <pr>h&eacute;tao</pr></trd>,
    <trd>&#32993;&#26691; <pr>h&uacute;t&aacute;o</pr></trd>,
    <trd>&#26680;&#26691;&#27877; <pr>h&eacute;t&aacute;on&iacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="juglan.0ujo0arbo">
  <kap><tld/>ujo, <var><kap><tld/>arbo</kap></var></kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="juglan.0ujo0arbo.genro">
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref>
       el familio <ref tip="malprt" cel="juglan1.0oj"><tld/>ujacoj</ref>
      (<trd lng="la">Juglans</trd>), arbo, kies plej konata specio
      (<trd lng="la">Juglans regia</trd>) estas
      kulturata pro la oleori&ccirc;aj man&gcirc;eblaj nuksoj, kaj la ligno
      uzata en meblofarado:
      <ekz>
        inter du <tld/>ujoj brilis la konstelacio de Oriono
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        pro ofta sterileco de <tld/>arboj en arbaroj, ursoj desupris de montaro,
        ser&ccirc;ante man&gcirc;a&jcirc;on en kampoj
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2014/011198.php"
              >Ursoj mortigataj</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la &hcirc;or]</klr>se&gcirc;aro <klr>[&#8230;]</klr> konsistas el <tld/>ujo-ligno kaj en &gcirc;i elstaras
        la se&gcirc;o de la episkopo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=508019">Nova Katedralo de
          Plasencia</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/jugland.jpg">maskla floro de
    Juglans regia
  </bld>
  <bld lok="../bld/jugland1.jpg">
    Juglans regia
  </bld>
  <trd lng="be">&c_a;&c_r;&c_e;&c_h;&c_a;&c_v;&c_a;&c_je;
&c_d;&c_r;&c_e;&c_v;&c_a;</trd>
  <trdgrp lng="br">
    <trd>krao&ntilde;enn<klr tip="ind"> (gwezenn)</klr></trd>,
    <trd>gwezenn-grao&ntilde;</trd>,
  </trdgrp>
  <trdgrp lng="ca">
    <trd>nouer</trd>,
    <trd>noguera</trd>
  </trdgrp>
  <trd lng="de">Walnussbaum</trd>
  <trd lng="en">walnut</trd>
  <trd lng="es">nogal</trd>
  <trd lng="fr">noyer</trd>
  <trd lng="hu">di&oacute;fa</trd>
  <trd lng="ja">&#12463;&#12523;&#12511;</trd>
  <trd lng="nl">notenboom</trd>
  <trd lng="pl">orzech w&lstroke;oski<klr> (drzewo)</klr></trd>
  <trd lng="pt">nogueira</trd>
  <trd lng="tr">ceviz</trd>
  <trd lng="ru">&c_o;&c_r;&c_je;&c_h;&c_o;&c_v;&c_o;&c_je;
&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  <trd lng="sv">valn&ouml;tstr&auml;d</trd>
  <trd lng="it">noce<klr> (albero)</klr></trd>
  <trd lng="sk">vla&scaron;sk&yacute; orech <klr>(strom)</klr></trd>
  <trdgrp lng="cs">
    <trd>o&rcaron;ech vla&scaron;sk&yacute;</trd>,
    <trd>o&rcaron;e&scaron;&aacute;k kr&aacute;lovsk&yacute;</trd>
  </trdgrp>
  <trd lng="zh">&#32993;&#26691; <pr>h&uacute;t&aacute;o</pr></trd>
</drv>

</art>
