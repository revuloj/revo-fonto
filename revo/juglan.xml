<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>jugland</rad>/o
</kap>
<drv mrk="juglan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Fruktokerno de <tld/>ujo.
    </dif>
  </snc>
  <trd lng="de">Walnu&szlig;</trd>
  <trd lng="fr">noix</trd>
  <trd lng="tr">ceviz</trd>
  <trd lng="ru">&c_g;&c_r;&c_je;&c_c;&c_k;&c_i;&c_j;
  &c_o;&c_r;&c_je;&c_h;</trd>
</drv>
<drv mrk="juglan.0ujo0arbo">
  <kap><tld/>ujo, <tld/>arbo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="juglan.0ujacoj"><tld/>ujacoj</ref>
      (<trd lng="la">Juglans</trd>), arbo, kies plej konata specio
      (<trd lng="la">Juglans regia</trd>) estas
      kulturata pro la oleori&ccirc;aj man&gcirc;eblaj kernoj, kaj la ligno
      uzata en meblofarado.
    </dif>
  </snc>
  <bld lok="../bld/jugland.jpg">maskla floro,
    (<trd lng="la">Juglans regia</trd>)
  </bld>
  <bld lok="../bld/jugland1.jpg">
    <trd lng="la">Juglans regia</trd>
  </bld>
  <trd lng="de">Walnu&szlig;baum</trd>
  <trd lng="fr">noyer</trd>
  <trd lng="tr">ceviz</trd>
  <trd lng="ru">&c_o;&c_r;&c_je;&c_h;&c_o;&c_v;&c_o;&c_je;
  &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
</drv>
<drv mrk="juglan.0ujacoj">
  <kap><tld/>ujacoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Juglandaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Walnu&szlig;gew&auml;chse</trd>
</drv>
</art>
