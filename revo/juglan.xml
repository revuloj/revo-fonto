<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>jugland</rad>/o
</kap>
<drv mrk="juglan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Nukso de <tld/>ujo. 
    </dif>
  </snc>
  <trd lng="de">Walnu&szlig;</trd>
  <trd lng="fr">noix</trd>
</drv>
<drv mrk="juglan.0ujo0arbo">
  <kap><tld/>ujo, <tld/>arbo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="jugla.0ujacoj"><tld/>ujacoj</ref>
      (<trd lng="la">Juglans</trd>), arbo, kies plej konata specio
      (<trd lng="la">Juglans regia</trd>) produktas oleori&ccirc;ajn
      man&gcirc;eblajn nuksojn, kaj kies lignon oni uzas por
      meblofarado.
    </dif>
  </snc>
  <bld lok="../bld/jugland.jpg">maskla floro, 
    (<trd lng="la">Juglans regia</trd>)</bld>
  <bld lok="../bld/jugland1.jpg"><trd lng="la">Juglans regia</trd></bld>
  <trd lng="de">Walnu&szlig;baum</trd>
  <trd lng="fr">noyer</trd>
</drv>
<drv mrk="juglan.0ujacoj">
  <kap><tld/>ujacoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj plantoj (<trd lng="la">Juglandaceae</trd>),
      al kiu apartenas la <ref tip="sub" cel="jugla.0ujo"><tld/>ujo</ref>.
    </dif>
  </snc>
  <trd lng="de">Walnu&szlig;gew&auml;chse</trd>
</drv>
</art>
