<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: visteri.xml,v 1.11 2023/09/05 09:43:56 revo Exp $">
<kap>
  <rad>visteri</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="visteri.0o">
  <kap><tld/>o</kap>
  <snc mrk="visteri.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> de lignaj lianoj,
      kultivataj
      pro siaj belaj floroj
      diverskoloraj, el &Ccirc;inujo, Japanujo, Koreujo kaj suda Usono, el
      familio <ref tip="malprt" cel="fabac.0oj">fabacoj</ref> (<trd lng="la">Wisteria</trd>):
      <ekz>
        en la &gcirc;ardeno staris pergolo de <tld/>o
        <fnt><bib>LaFamilio</bib>, <lok>&ccirc;apitro 16a, p.
        199a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">glicina</trd>
  <trd lng="de">Wisteria</trd>
  <trd lng="fa">&f_g;&a_l;&f_y;&a_s;&f_y;&a_n;</trd>
  <trd lng="fr">glycine</trd>
  <trd lng="hu">lilaak&aacute;c</trd>  
  <trd lng="ja">&#x30D5;&#x30B8;  [<ind>&#x3075;&#x3058;</ind>]</trd>
  <trdgrp lng="pt">
   <trd>glic&iacute;nia</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_l;&c_i;&c_c;&c_i;&c_n;&c_i;&c_ja;</trd>
  <trd lng="uk">&#x432;&#x456;&#x441;&#x442;&#x435;&#x440;&#x456;&#x44F;</trd>
</drv>
</art>

