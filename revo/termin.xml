<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>termin</rad>/o
</kap>
<drv mrk="termin.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">LIN</uzo>
  <snc>
    <dif>
      Vorto a&ubreve; esprimo speciala al iu scienco, arto, metio, fako.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Ausdruck</trd>,
    <trd>Terminus</trd>
  </trdgrp>
  <trd lng="es">t&eacute;rmino</trd>
  <trd lng="fr">terme</trd>
  <trdgrp lng="hu">
    <trd>szaksz&oacute;</trd>,
    <trd>terminus</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>term</trd>,
    <trd>vakterm</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_r;&c_m;&c_i;&c_n;</trd>
</drv>

<drv mrk="termin.0aro">
  <kap><tld/>aro</kap>
  <uzo tip="fak">LIN</uzo>
  <uzo tip="fak">SCI</uzo>
  <snc>
    <dif>
      Speciala leksiko de iu scienco, arto, metio, fako.
    </dif>
    <trd lng="hu">terminol&oacute;gia</trd>
    <trd lng="ru">&c_t;&c_je;&c_r;&c_m;&c_i;&c_n;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  </snc>
  <snc>
    <dif>
      Faka vortaro.
    </dif>
    <ref tip="super" cel="vort.0aro">vortaro</ref>
    <refgrp tip="vid">
      <ref cel="leksik1.0o">leksikono</ref>,
      <ref cel="glos.0aro">glosaro</ref>
    </refgrp>
    <trd lng="ru">&c_s;&c_l;&c_o;&c_v;&c_a;&c_r;&c_mol; &c_t;&c_je;&c_r;&c_m;&c_i;&c_n;&c_o;&c_v;</trd>
  </snc>
  <ref tip="vid" cel="tezaux.0o">teza&ubreve;ro</ref>
  <trd lng="es">diccionario especializado</trd>
  <trd lng="fr">lexique</trd>
  <trdgrp lng="nl">
    <trd>vakwoordenlijst</trd>,
    <trd>vakwoordenboek</trd>
  </trdgrp>
  <trd lng="hu">szaksz&oacute;t&aacute;r</trd>
</drv>

<drv mrk="termin.0ologio">
  <kap><tld/>ologio</kap>
  <uzo tip="fak">LIN</uzo>
  <snc>
    <dif>
      Lingvoscienca fako studanta fakajn lingvojn kaj
      terminarojn.
    </dif>
    <trd lng="es">terminolog&iacute;a</trd>
    <trd lng="hu">terminol&oacute;gia</trd>
    <trd lng="nl">terminologie</trd>
    <trd lng="ru">&c_t;&c_je;&c_r;&c_m;&c_i;&c_n;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  </snc>
</drv>
</art>
