<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>termin</rad>/o
    </kap>
    <drv mrk="termin.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <uzo tip="fak">LIN</uzo>
      <snc>
        <dif>
          Vorto a&ubreve; esprimo speciala al iu scienco, arto, metio, fako.  
        </dif>
      </snc>
      <trd lng="es">t&eacute;rmino</trd>
      <trd lng="fr">terme</trd>
    </drv>
    <drv mrk="termin.0aro">
      <kap><tld/>aro</kap>
      <uzo tip="fak">LIN</uzo>
      <uzo tip="fak">SCI</uzo>
      <snc>
        <dif>
          Speciala leksiko de iu scienco, arto, metio, fako.
        </dif>
        <trd lng="ru">&#x442;&#x435;&#x440;&#x43c;&#x438;&#x43d;&#x43e;&#x43b;&#x43e;&#x433;&#x438;&#x44f;</trd>
      </snc>
      <snc>
        <dif>
          Faka vortaro.
        </dif>
        <ref tip="super" cel="vort.0aro">vortaro</ref>
        <refgrp tip="vid"><ref cel="leksik1.0o">leksikono</ref>,
        <ref cel="glos.0aro">glosaro</ref></refgrp>
        <trd lng="ru">&#x441;&#x43b;&#x43e;&#x432;&#x430;&#x440;&#x44c; &#x442;&#x435;&#x440;&#x43c;&#x438;&#x43d;&#x43e;&#x432;</trd>
      </snc>
      <ref tip="vid" cel="tezaux.0o">teza&ubreve;ro</ref>
      <trd lng="es">diccionario especializado</trd>
      <trd lng="fr">lexique</trd>
    </drv>
    <drv mrk="termin.0ologio">
      <kap><tld/>ologio</kap>
      <uzo tip="fak">LIN</uzo>
      <snc>
        <dif>
          Lingvoscienca fako studanta fakajn lingvojn kaj
          terminarojn.
        </dif>
      <trd lng="es">terminolog&iacute;a</trd>
        <trd lng="ru">&#x442;&#x435;&#x440;&#x43c;&#x438;&#x43d;&#x43e;&#x43b;&#x43e;&#x433;&#x438;&#x44f;</trd>
      </snc>
    </drv>
  </art>

