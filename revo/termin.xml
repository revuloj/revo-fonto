<drv mrk="termin.0aristo">
  <kap><tld/>aristo</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="termin.0aristo.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="super" cel="lingv.0isto">Lingvisto</ref> studanta fakajn
      <tld/>oj, kompilanta <tld/>arojn:
      <ekz>
        <nom>Eurodicautom</nom> <klr>[&#x2026;]</klr> povas esti interesa por
        tradukistoj / vortaristoj / <tld/>aristoj
        <fnt>
        <aut>luisgui</aut>,
        <vrk>12-lingva datumbazo (Eurodicautom)</vrk>,
        <lok>soc.culture.esperanto, 1999-05-24</lok>
        </fnt>.
      </ekz>
      <ekz>
        <nom>Christian Bertin</nom> <klr>[&#x2026;]</klr>, esperanta <tld/>aristo, vivanta en Bretonio
        <fnt>
          <vrk><url ref="&Viki;/Christian_Bertin">Christian Bertin -
          Vikipedio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_k;&c_l;&c_a;&c_d;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k; &c_t;&c_e;&c_r;&c_m;&c_ib;&c_n;&c_a;&c_l;&c_a;&c_g;&c_ib;&c_ch;&c_n;&c_a;&c_g;&c_a; &c_s;&c_l;&c_o;&c_w;&c_n;&c_ib;&c_k;&c_a;</trd>
