<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kolor</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kolor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kolor.0o.propre">
    <dif>
      Aparta impreso farita sur la okulo de lumradio per la spektro de
      &gcirc;iaj ondolongoj (inter 380 kaj 760&#8239;nm):
      <ekz>
        la sep <tld/>oj de la &ccirc;ielarko;
      </ekz>
      <ekz>
        ru&gcirc;eta <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        flavru&gcirc;a <tld/>o
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;tono havas <tld/>on blanka-bluan
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        blanko, grizo kaj nigro estas ne&ubreve;tralaj <tld/>oj;
      </ekz>
      <ekz>
        <ind>kiam nokto vualas, &ccirc;iuj <tld/>oj egalas</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="ca">de nit tos els gats s&oacute;n negres</trd>
        <trd lng="de">nachts sind alle Kstzen grau</trd>
        <trd lng="fr">la nuit, tous les <ind>chat</ind>s sont gris</trd>
        <trd lng="pl">noc&aogonek; wszystkie koty s&aogonek; czarne
        </trd>
      </ekz>
      <ekz>
        sento de feli&ccirc;eco, kiu montras al ili &ccirc;ion kun
        la plej agrablaj <tld/>oj
        <klr>(&scirc;ajno, aspekto)</klr>;
      </ekz>
      <ekz>
        bela desegno kaj belegaj <tld/>oj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        brilaj <tld/>oj
        <fnt>
          <bib>FdO</bib>
        </fnt>;
      </ekz>
      <ekz>
        la pala <tld/>o de la viza&gcirc;o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        haroj de nedifinita <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la ruso havis &ccirc;evalon <klr>[&#8230;]</klr> de <tld/>o blanka, la tataro havis bonan
        nigran &ccirc;evalon
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">color</trd>
    <trdgrp lng="pl">
      <trd>kolor</trd>,
      <trd>barwa</trd>
    </trdgrp>
    <trd lng="ru">&c_c;&c_v;&c_je;&c_t;</trd>
  </snc>
  <snc mrk="kolor.0o.KLA">
    <dif>
      Difinita <tld/>o rolanta kiel konvencia signo.
    </dif>
    <subsnc mrk="kolor.0o.POL">
      <dif>
        <tld lit="K"/>a simbolo de iu homgrupo, teamo:
        <ekz>
          sektoj el &ccirc;iuj <tld/>oj
          <fnt>
            <bib>Monato</bib>
            <aut>Gilbert Ledon</aut>
            <vrk><url ref="https://www.monato.be/2003/008040.html">Kvina povo</url></vrk>
            <lok>2003</lok>
          </fnt>;
        </ekz>
        <ekz>
          administrantoj estas enpostenigitaj la&ubreve; politika
          <tld/>o <klr>(opinio, partio)</klr>, kaj ne la&ubreve; kapabloj
          <fnt>
            <bib>Monato</bib>
            <aut>Christian Fabossou</aut>
            <vrk><url ref="http://steloj.de/esperanto/monato2/007042.html">Afriko: &Scirc;an&gcirc;oj &dash; ankora&ubreve; ne sufi&ccirc;aj</url></vrk>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="kolor.0o.ludkarta">
      <uzo tip="fak">SPO</uzo>
      <dif>
        &Ccirc;iu el la du grupoj de la ludkartaj
        <ref tip="sub" cel="emblem.0o.karta">emblemoj</ref>,
        la ru&gcirc;a (karoo kaj kero) kaj la nigra (piko kaj trefo).
      </dif>
      <trd lng="ca">color <klr>(ex: escala de color, al
      p&ograve;quer)</klr></trd>
      <trd lng="pl">kolor</trd>
      <trd lng="ru">&c_c;&c_v;&c_je;&c_t;</trd>
    </subsnc>
  </snc>
  <snc mrk="kolor.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Sensebla ecaro, pro kiu io distingi&gcirc;as
      kaj rekoneblas:
      <ekz>
        la iom laca <tld/>o de lia viza&gcirc;o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        neniam homaranoj trudu la gentan <tld/>on de sia vida&jcirc;o al ceteraj
        <fnt>
          <bib>VivZam</bib>
        </fnt>.
      </ekz>
    </dif>
    <subsnc mrk="kolor.0o.stilo">
      <uzo tip="fak">BELE</uzo>
      <dif>
        <ref tip="dif" cel="stil.0o">Stilo</ref>:
        <ekz>
          la malofteco de la adjektivoj donas al lia lingvo
          tre kuriozan <tld/>on;
        </ekz>
        <ekz>
          sen<tld/>a oficiala parolo
          <fnt>
            <bib>Paroloj</bib>
            <vrk>Parolado anta&ubreve; la Dua Kongreso Esperantista en Gen&egrave;ve en la 28a de a&ubreve;gusto 1906</vrk>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="ca">
        <trd>forma</trd>,
        <trd>estil</trd>,
        <trd>expressi&oacute;</trd>
      </trdgrp>
      <trd lng="pl">koloryt</trd>
      <trd lng="ru">&c_k;&c_o;&c_l;&c_o;&c_r;&c_i;&c_t;</trd>
    </subsnc>
    <subsnc mrk="kolor.0o.sonkoloro">
      <uzo tip="fak">MUZ</uzo>
      <ref tip="dif" cel="tembr.0o.MUZ">tembro</ref>
      <trd lng="ca">timbre</trd>
      <trd lng="pl">barwa g&lstroke;osu</trd>
      <trd lng="ru">&c_o;&c_k;&c_r;&c_a;&c_s;&c_k;&c_a;</trd>
    </subsnc>
  </snc>
  <trd lng="bg">&c_c;&c_v;&c_ja;&c_t;</trd>
  <trd lng="br">liv</trd>
  <trdgrp lng="cs">
    <trd>barva</trd>,
    <trd>instrukce pro nastaven&iacute; barevn&eacute;ho
    v&yacute;stupu</trd>
  </trdgrp>
  <trd lng="de">Farbe</trd>
  <trd lng="en">color</trd>
  <trd lng="es">color</trd>
  <trdgrp lng="fr">
    <trd>couleur</trd>,
    <trd>coloris</trd>,
    <trd>teinte<klr tip="ind"> (coloris)</klr></trd>,
    <trd>ton<klr tip="ind"> (coloris)</klr></trd>
  </trdgrp>
  <trd lng="id">warna</trd>
  <trd lng="nl">kleur</trd>
  <trdgrp lng="pt">
    <trd>cor</trd>,
    <trd>colorido</trd>,    
    <trd>timbre <klr tip="amb">(de voz)</klr></trd>
  </trdgrp>
  <trd lng="sk">farebn&yacute; odtie&ncaron;</trd>
  <trd lng="sv">f&auml;rg</trd>
  <trd lng="tp">kule</trd>
  <trd lng="tr">renk</trd>
</drv>

<drv mrk="kolor.0a">
  <kap><tld/>a</kap>
  <snc mrk="kolor.0a.havanta">
    <dif>
      Havanta <tld/>on:
      <ekz>
        <tld/>aj bildoj;
      </ekz>
      <ekz>
        briko<tld/>a muro
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        bel<tld/>aj floroj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        rozo<tld/>a bu&scirc;o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        pajlo<tld/>a
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        ar&gcirc;ent<tld/>aj haroj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        cindro<tld/>a vesto
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        pentra&jcirc;oj flam<tld/>aj
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>sang<tld/>a</ind>;
        <trd lng="fr">couleur <ind>sang</ind></trd>
        <trd lng="nl">bloedrood</trd>
        <trd lng="pl">krwistoczerwony</trd>
      </ekz>
      <ekz>
        <ind>unu<tld/>a</ind>;
        <trd lng="ca">unicolor</trd>
        <trd lng="de">einfarbig</trd>
        <trd lng="fr">monochrome</trd>
        <trd lng="id">monokrom</trd>
        <trdgrp lng="nl">
          <trd>eenkleurig</trd>,
          <trd>monochroom</trd>
        </trdgrp>
        <trdgrp lng="pl">
          <trd>jednobarwny</trd>,
          <trd>monochromatyczny</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind>du<tld/>a</ind>;
        <trd lng="ca">bicolor</trd>
        <trd lng="de">zweifarbig</trd>
        <trd lng="fr">bicolore</trd>
        <trd lng="id">dwiwarna</trd>
        <trd lng="nl">tweekleurig</trd>
        <trd lng="pl">dwubarwny</trd>
      </ekz>
      <ekz>
        <ind>tri<tld/>a</ind>.
        <trdgrp lng="ca">
          <trd>tricolor</trd>,
          <trd>tricrom&agrave;tic</trd>
        </trdgrp>
        <trd lng="de">dreifarbig</trd>
        <trd lng="fr">tricolore</trd>
        <trd lng="id">triwarna</trd>
        <trd lng="nl">driekleurig</trd>
        <trd lng="pl">tr&oacute;jbarwny</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_o;&c_c;&c_v;&c_je;&c_t;&c_je;&c_n;</trd>,
    <trd>&c_c;&c_v;&c_je;&c_t;&c_n;&c_o;&c_k;&c_o;&c_zh;</trd>
  </trdgrp>
  <trd lng="ca">colorat</trd>
  <trdgrp lng="cs">
    <trd>barevn&yacute;</trd>,
    <trd>barvit&yacute;</trd>
  </trdgrp>
  <trd lng="de">farbig</trd>
  <trd lng="en">colored</trd>
  <trdgrp lng="fr">
    <trd>color&eacute;</trd>
  </trdgrp>
  <trd lng="id">[<ind>warna</ind>] berwarna</trd>
  <trdgrp lng="nl">
    <trd>gekleurd</trd>,
    <trd>kleurig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kolorowy</trd>,
    <trd>barwny</trd>
  </trdgrp>
  <trd lng="ru">&c_c;&c_v;&c_je;&c_t;&c_n;&c_o;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>farbist&yacute;</trd>,
    <trd>pestrofarebn&yacute;</trd>,
    <trd>pestr&yacute;</trd>,
    <trd>viacfarebn&yacute;</trd>
  </trdgrp>
  <trd lng="tp">kule</trd>
  <trdgrp lng="tr">
    <trd>renkli</trd>,
    <trd>boyal&inodot;</trd>
  </trdgrp>
</drv>

<drv mrk="kolor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kolor.0i.doni">
    <dif>
      Doni <tld/>on al, ka&ubreve;zi <tld/>on &ccirc;e:
      <ekz>
        la grenoj flave <tld/>as la kampon;
      </ekz>
      <ekz>
        la suno <tld/>as la nubojn;
      </ekz>
      <ekz>
        la tempo grize <tld/>is liajn harojn;
      </ekz>
      <ekz>
        sur la portretoj el Kembri&gcirc;o espera &gcirc;ojo 
        <tld/>as la zorgemon en okuloj kaj sur
        lipoj de la Majstro
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la akompananta muziko <tld/>as la tutan dialogon.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_o;&c_c;&c_v;&c_je;&c_t;&c_ja;&c_v;&c_a;&c_m;</trd>
  <trd lng="ca">colorar</trd>
  <trd lng="cs">kolorovat</trd>
  <trd lng="de">f&auml;rben</trd>
  <trd lng="en">color</trd>
  <trd lng="fr">colorer</trd>
  <trd lng="id">[<ind>warna</ind>] mewarnai</trd>
  <trdgrp lng="nl">
    <trd>kleuren</trd>,
    <trd>verven</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kolorowa&cacute;</trd>,
    <trd>koloryzowa&cacute;</trd>,
    <trd>barwi&cacute;</trd>,
    <trd>ubarwia&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_k;&c_r;&c_a;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_k;&c_r;&c_a;&c_s;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">kolorova&tcaron;</trd>
  <trd lng="sv">f&auml;rga</trd>
  <trd lng="tp">kule</trd>
  <trdgrp lng="tr">
    <trd>boyamak</trd>,
    <trd>renk vermek</trd>
  </trdgrp>
</drv>

<drv mrk="kolor.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="kolor.0igi.sursxmiri">
    <dif>
      Sur&scirc;miri ion (desegnon, gravura&jcirc;on, paperon,
      tola&jcirc;on...) per <tld/>iloj:
      <ekz>
        vir&scirc;afaj feloj ru&gcirc;e <tld/>igitaj
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 25:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>igis sian viza&gcirc;on kaj vestis sin kiel
        maljuna butikistino
        <fnt><bib>ElektFab</bib>, <lok>Ne&gcirc;ulino</lok></fnt>;
      </ekz>
      <ekz>
        fenestrojn <klr>[mi]</klr> <tld/>igas per cinabro
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 22:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        flor&gcirc;ardeneto kun bele <tld/>igita palisaro
        <fnt>
          <bib>Fab1</bib>
          <lok>Lekanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la suno min brul<tld/>igis
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 1:6</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_b;&c_o;&c_ja;&c_d;&c_i;&c_s;&c_v;&c_a;&c_m;</trd>
  <trd lng="br">liva&ntilde;</trd>
  <trd lng="ca">acolorir</trd>
  <trdgrp lng="cs">
    <trd>nechat nat&rcaron;&iacute;t</trd>,
    <trd>nechat omalovat</trd>
  </trdgrp>
  <trd lng="de">bemalen</trd>
  <trd lng="en">color</trd>
  <trd lng="fr">colorier</trd>
  <trdgrp lng="nl">
    <trd>kleuren</trd>,
    <trd>verven</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kolorowa&cacute;</trd>,
    <trd>barwi&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_a;&c_s;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_k;&c_r;&c_a;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_k;&c_r;&c_a;&c_s;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nafarbi&tcaron;</trd>,
    <trd>vyma&lcaron;ova&tcaron;</trd>,
    <trd>zafarbi&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">m&aring;la</trd>
  <trdgrp lng="tr">
    <trd>renklendirmek</trd>,
    <trd>boyamak</trd>
  </trdgrp>
</drv>

<drv mrk="kolor.0igilo">
  <kap><tld/>igilo</kap>
  <snc mrk="kolor.0igilo.farbo">
    <dif>
      <ref tip="dif" cel="farb.0o">Farbo</ref>:
      <ekz>
        ili nur zorgas pri kostumoj, <tld/>igiloj kaj parfumoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXV</lok>
        </fnt>;
      </ekz>
      <ekz>
        desegna&jcirc;oj, faritaj sur papero per <tld/>igiloj
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 6a</lok></fnt>;
      </ekz>
      <ekz>
        la komenci&gcirc;anta produktado de artefaritaj feroksidoj kaj kemiaj <tld/>igiloj
        negative efikis la industrion pri natura okro
        <fnt>
          <bib>Monato</bib>
          <aut>Robert Roze</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006945.html">Francio: Flagrantaj koloroj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_b;&c_o;&c_ja;</trd>
  <trdgrp lng="ca">
    <trd>pintura</trd>,
    <trd>colorant</trd>
  </trdgrp>
  <trd lng="cs">barva</trd>
  <trd lng="de">Farbe</trd>
  <trd lng="en">paint</trd>
  <trdgrp lng="fr">
    <trd>peinture<klr tip="ind"> (produit &agrave; base de
    pigments)</klr></trd>,
    <trd>couleur<klr tip="ind"> (peinture)</klr></trd>
  </trdgrp>
  <trd lng="id">cat</trd>
  <trd lng="nl">verf</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_a;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_s;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">farba</trd>
  <trd lng="tr">boya</trd>
</drv>

<drv mrk="kolor.0igisto">
  <kap><tld/>igisto</kap>
  <snc mrk="kolor.0igisto.profesio">
    <dif>
      Homo, kies metio estas <tld/>igi:
    </dif>
    <subsnc mrk="kolor.0igisto.BELA">
      <uzo tip="fak">BELA</uzo>
      <dif>
        Sur&scirc;mirante desegna&jcirc;ojn, manuskriptojn ktp per <tld/>iloj:
        <ekz>
          la <tld/>igisto havas fingrojn, kiuj malbonodoras
          <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 1a</lok></fnt>.
        </ekz>
      </dif>  
      <trd lng="ca">colorista</trd>
      <trd lng="fr">coloriste</trd>
      <trdgrp lng="tr">
        <trd>renk sanat&ccedil;&inodot;s&inodot;</trd>,
        <trd>renk&ccedil;i</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="kolor.0igisto.TEKS">
      <uzo tip="fak">TEKS</uzo>
      <dif>
        <tld lit="K"/>igante &scirc;tofon, drapon.
      </dif>
      <trd lng="ca">tintorer</trd>
      <trd lng="de">F&auml;rber</trd>
      <trd lng="pl">farbiarz</trd>
    </subsnc>
    <subsnc mrk="kolor.0igisto.kosmetiko">
      <dif>
        <tld lit="K"/>igante ungojn, harojn:
        <ekz>
          piedungo<tld/>igistino
          <fnt>
            <bib>Monato</bib>
            <aut>Franz-Georg R&ouml;ssler</aut>
            <vrk><url ref="https://www.monato.be/2013/010812.php">Po&scirc;to perdita</url></vrk>
            <lok>2013</lok>
          </fnt>.
        </ekz>
      </dif>     
    </subsnc>
    <subsnc mrk="kolor.0igisto.KON">
      <uzo tip="fak">KON</uzo>
      <dif>
        <ref tip="dif" cel="farb.0isto">Farbisto</ref>:
        <ekz>
          li ellernis la farbistan metion <klr>[&#8230;]</klr> kaj poste laboris kiel
          farbisto-<tld/>igisto en diversaj konstrufirmaoj en Vieno
          <fnt>
            <bib>Monato</bib>
            <aut>Julius Hauser</aut>
            <vrk><url ref="https://www.monato.be/2014/011335.php">Pentristo de la homo kaj de la naturo</url></vrk>
            <lok>2014</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <trd lng="de">Illustrator</trd>
  </snc>
  <trd lng="cs">mal&iacute;&rcaron; pokoj&uring;</trd>
  <trd lng="nl">verver</trd>
  <trd lng="sk">maliar izieb</trd>
</drv>

<drv mrk="kolor.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="kolor.0ilo.farbo">
    <dif>
      <ref tip="dif" cel="farb.0o">Farbo</ref>:
      <ekz>
        pala <tld/>ilo ne sufi&ccirc;as, por kopii la &ccirc;ielan spiriton, kiu regis en lia fajra
        okulo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        son&gcirc;oj pentris pejza&gcirc;on per <tld/>iloj el smeraldoj,
        ar&gcirc;ento, rubenoj
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 3a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">pintura</trd>
  <trd lng="cs">barva <klr>(n&aacute;t&ecaron;r.hmota)</klr></trd>
  <trd lng="de">Farbe</trd>
  <trd lng="en">paint</trd>
  <trdgrp lng="fr">
    <trd>peinture<klr tip="ind"> (produit &agrave; base de
    pigments)</klr></trd>,
    <trd>couleur<klr tip="ind"> (peinture)</klr></trd>
  </trdgrp>
  <trd lng="id">cat</trd>
  <trd lng="nl">verf</trd>
  <trdgrp lng="pl">
    <trd>barwnik</trd>,
    <trd>pigment</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_a;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_s;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">farba <klr>(n&aacute;terov&aacute; hmota)</klr></trd>
  <trd lng="tr">boya</trd>
</drv>

<drv mrk="kolor.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="kolor.0isto.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Pentristo, kiu abunde, lerte uzas <tld/>ojn,
      pli ol desegnajn liniojn; <tld/>isma pentristo.
    </dif>
    <trd lng="de">Kolorist</trd>
  </snc>
  <snc mrk="kolor.0isto.profesio">
    <dif>
      Iu, kiu profesie provizas artverkojn per ta&ubreve;gaj koloroj:
      <ekz>
        mi ser&ccirc;as redaktiston kaj <tld/>iston por simpla muzikfilmeto
        <fnt>
          <vrk><url ref="https://eo.broadcastbeat.com/redaktista-koloristo-3/">Redaktisto / Koloristo </url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Zeichner</trd>,
      <trd>Colorist</trd>
    </trdgrp>
  </snc>
  <trd lng="br">liver</trd>
  <trd lng="ca">colorista</trd>
  <trd lng="cs">mal&iacute;&rcaron; pokoj&uring;</trd>
  <trd lng="en">colorist</trd>
  <trd lng="fr">coloriste</trd>
  <trd lng="nl">colorist</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_o;&c_r;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">maliar izieb</trd>
</drv>

<drv mrk="kolor.divers0a">
  <kap>divers<tld/>a,
  <var><kap>miks<tld/>a</kap></var></kap>
  <snc mrk="kolor.divers0a.KOMUNE">
    <dif>
      Havanta plurajn malsamajn <tld/>ojn:
      <ekz>
        &scirc;i havis sur si divers<tld/>an veston
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 13:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;afo miks<tld/>a
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 30:32</lok>
        </fnt>;
      </ekz>
      <ekz>
        divers<tld/>aj papilioj
        <fnt><bib>Fab1</bib>, <lok>Elinjo-fingreto</lok></fnt>;
      </ekz>
      <ekz>
        retoj eltran&ccirc;itaj el divers<tld/>a papero
        <fnt>
          <bib>Fab2</bib>
          <lok>Abio</lok>
        </fnt>;
      </ekz>
      <ekz>
        la miks<tld/>a blazono sur la malnova urbodomo lumis
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>policrom</trd>,
    <trd>virolat</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>r&uring;znobarevn&yacute;</trd>,
    <trd>strakat&yacute;</trd>
  </trdgrp>
  <trd lng="de">verschiedenfarbig</trd>
  <trdgrp lng="fr">
    <trd>multicolore</trd>,
    <trd>polychrome</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>warna</ind>-warni</trd>,
    <trd>[<ind>warna</ind>] berwarna-warna</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>veelkleurig</trd>,
    <trd>polychroom</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wielobarwny</trd>,
    <trd>kolorowy</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>r&ocirc;znofarebn&yacute;</trd>,
    <trd>strakat&yacute;</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>&ccedil;ok renkli</trd>,
    <trd>reng&acirc;renk</trd>
  </trdgrp>
</drv>

<drv mrk="kolor.dom0igisto">
  <kap>dom<tld/>igisto,
  <var><kap>mur<tld/>igisto</kap></var></kap>
  <snc mrk="kolor.dom0igisto.KON">
    <uzo tip="fak">KON</uzo>
    <ref tip="dif" cel="farb.0isto">farbisto</ref>
  </snc>
  <trd lng="ca">pintor <klr>(de parets)</klr></trd>
  <trd lng="de">Maler <klr>(Handwerker)</klr></trd>
  <trd lng="en">housepainter</trd>
  <trd lng="fr">peintre en b&acirc;timent</trd>
  <trdgrp lng="id">
    <trd>[<ind>cat</ind>] pengecat</trd>,
    <trd><ind>tukang</ind> cat</trd>
  </trdgrp>
  <trd lng="nl">huisschilder</trd>
  <trd lng="pl">malarz<klr> pokojowy</klr></trd>
  <trd lng="ru">&c_m;&c_a;&c_l;&c_ja;&c_r;</trd>
  <trd lng="tr">boyac&inodot;</trd>
</drv>

<drv mrk="kolor.mult0a">
  <kap>mult<tld/>a, <var><kap>multe<tld/>a</kap></var></kap>
  <snc mrk="kolor.mult0a.KOMUNE">
    <dif>
      Havanta multajn kontrastajn kolorojn:
      <ekz>
        multe<tld/>a papilio
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la duonpatrino <klr>[&#8230;]</klr> montris la&ccirc;on plektitan el
        mult<tld/>a silko
        <fnt><bib>ElektFab</bib>, <lok>Ne&gcirc;ulino</lok></fnt>;
      </ekz>
      <ekz>
        la &gcirc;ardenfloroj mult<tld/>as ekde frua printempo
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="https://www.monato.be/2004/008267.html">Estigi lagetojn</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_k;&c_o;&c_l;&c_o;&c_r;&c_i;&c_t;&c_je;&c_n;</trd>
  <trd lng="ca">multicolor</trd>
  <trdgrp lng="cs">
    <trd>mnohobarevn&yacute;</trd>,
    <trd>pestrobarevn&yacute;</trd>
  </trdgrp>
  <trd lng="de">bunt</trd>
  <trdgrp lng="fr">
    <trd>chamarr&eacute;</trd>,
    <trd>multicolore</trd>,
  </trdgrp>
  <trd lng="id"><ind>warna</ind>-warni</trd>
  <trdgrp lng="nl">
    <trd>veelkleurig</trd>,
    <trd>polychroom</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wielobarwny</trd>,
    <trd>kolorowy</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>farbist&yacute;</trd>,
    <trd>pestrofarebn&yacute;</trd>,
    <trd>pestr&yacute;</trd>,
    <trd>viacfarebn&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="kolor.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="kolor.sen0a.KOMUNE">
    <dif>
      Ne havanta <tld/>on, tio estas a&ubreve; blanka, griza, nigra,
      a&ubreve; tute diafana:
      <ekz>
        maldika sen<tld/>a hararo
        <fnt><bib>Kiso</bib>, <lok>&Aring;ge</lok></fnt>;
      </ekz>
      <ekz>
        sen<tld/>a lako
        <fnt>
        <aut>M. Belosevic</aut>,
        <vrk>-</vrk>,
        <lok>soc.culture.esperanto, 1997-03-28</lok></fnt>;
      </ekz>
      <ekz>
        la sen<tld/>i&gcirc;intaj pupiloj de la maljunulino ne malseki&gcirc;is per larmo
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_b;&c_je;&c_z;&c_c;&c_v;&c_je;&c_t;&c_je;&c_n;</trd>
  <trd lng="ca">incolor</trd>
  <trd lng="cs">bezbarv&yacute;</trd>
  <trd lng="de">farblos</trd>
  <trd lng="en">colorless</trd>
  <trd lng="fr">incolore</trd>
  <trdgrp lng="id">
    <trd><ind>tanpa</ind> warna</trd>,
    <trd>takberwarna</trd>
  </trdgrp>
  <trd lng="nl">kleurloos</trd>
  <trd lng="pl">bezbarwny</trd>
  <trd lng="sk">bezfarebn&yacute;</trd>
</drv>

<drv mrk="kolor.son0o">
  <kap>son<tld/>o</kap>
  <snc mrk="kolor.son0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="dif" cel="tembr.0o.MUZ">Tembro</ref>:
      <ekz>
        efektdampiloj <klr>[&#8230;]</klr> &scirc;an&gcirc;as precipe la son<tld/>on
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=282654"
              >Dampilo (muzikinstrumento)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_t;&c_je;&c_m;&c_b;&c_malmol;&c_r;</trd>
  <trd lng="ca">timbre</trd>
  <trdgrp lng="cs">
    <trd>barva hlasu</trd>,
    <trd>barva zvuku</trd>
  </trdgrp>
  <trd lng="de">Klangfarbe</trd>
  <trd lng="en">timbre</trd>
  <trdgrp lng="fr">
    <trd>couleur musicale</trd>,
    <trd>timbre<klr tip="ind"> (sonore)</klr></trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>timbre</trd>,
    <trd><ind>warna</ind> nada</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>muziekkleur</trd>,
    <trd>timbre</trd>
  </trdgrp>
  <trd lng="pl">barwa g&lstroke;osu</trd>
  <trd lng="ru">&c_o;&c_k;&c_r;&c_a;&c_s;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>farba hlasu</trd>,
    <trd>farba zvuku</trd>
  </trdgrp>
</drv>

<drv mrk="kolor.sxangx0a">
  <kap>&scirc;an&gcirc;<tld/>a</kap>
  <snc mrk="kolor.sxangx0a.KOL">
    <dif>
      Kies <tld/>o facile &scirc;an&gcirc;i&gcirc;as la&ubreve; rebriloj
      de la lumo a&ubreve; pro alia ka&ubreve;zo:
      <ekz>
        komenci&gcirc;is traveturado de la a&ubreve;gustanoj &mdash;
        vicaro impona, brila, &scirc;an&gcirc;<tld/>a, kiel serpento
        <fnt>
          <aut>H. Sienkiewicz, trad. Lydia Zamenhof</aut>
          <vrk>Quo vadis?</vrk>
          <lok>1933</lok>
        </fnt>.
      </ekz>     
    </dif>
  </snc>
  <trd lng="ca">tornassolat</trd>
  <trd lng="fr">chatoyant</trd>
  <trd lng="nl">kleurrijk</trd>
</drv>

<drv mrk="kolor.tri0eto">
  <kap>tri<tld/>eto</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="kolor.tri0eto.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="dif" cel="viol.0ego.HOR">Violego</ref>:
      <ekz>
        ili prenis kun si anka&ubreve; la malestimatan kalendulon kaj la sova&gcirc;an tri<tld/>eton
        <fnt>
          <bib>Fab2</bib>
          <lok>An&gcirc;elo</lok>
        </fnt>;
      </ekz>
      <ekz>
        alpaj edelvejsoj kaj moder-klimataj tri<tld/>etoj kune
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj'</aut>
          <vrk><url ref="https://www.monato.be/2015/011315.php">La islanda: revo de ne plu juna lernemulo</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#8230;]</klr> veturis sur la tri<tld/>etoj
        &ccirc;irka&ubreve; la monumento 
        <fnt>
          <aut>A. Gregor, trad. J. Chvosta</aut>,
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr46/auto.html">
          A&ubreve;to-ekskurso</url></vrk>,
          <lok>Nica Literatura Revuo, 1959-07 &gcirc;is 08 (4/6), p.
          294-298a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_t;&c_je;&c_m;&c_je;&c_n;&c_u;&c_g;&c_i;</trd>
  <trdgrp lng="cs">
    <trd>mace&scaron;ka</trd>,
    <trd>mace&scaron;ka trojbarevn&aacute;</trd>
  </trdgrp>
  <trd lng="de">Stiefm&uuml;tterchen</trd>
  <trd lng="en">pansy</trd>
  <trd lng="fa">&a_b;&a_n;&a_f;&a_sh;&a_h;&a_hamza_sure;
  &a_s;&a_h;&zwnj;&a_r;&a_n;&f_g;</trd>
  <trd lng="fr">pens&eacute;e<klr> (botanique)</klr></trd>
  <trd lng="hu">&aacute;rv&aacute;cska</trd>
  <trd lng="nl">driekleurig <ind>viooltje</ind></trd>
  <trdgrp lng="pl">
    <trd>bratek</trd>,
    <trd>fio&lstroke;ek tr&oacute;jbarwn</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_jo;&c_h;&c_c;&c_v;&c_je;&c_t;&c_n;&c_a;&c_ja;
    <ind>&c_f;&c_i;&c_a;&c_l;&c_k;&c_a;</ind></trd>,
    <trd>&c_a;&c_n;&c_ju;&c_t;&c_i;&c_n;&c_y;
    &c_g;&c_l;&c_a;&c_z;&c_k;&c_i;</trd>
  </trdgrp>
  <trd lng="sk">sir&ocirc;tka</trd>
  <trd lng="sv">styvmorsviol</trd>
</drv>

<drv mrk="kolor.tri0o">
  <kap>
    tri<tld/>o
  </kap>
  <snc mrk="kolor.tri0o.flago">
    <dif>
      Flago tri<tld/>a:
      <ekz>
        tri<tld/>oj kun belaj viol<tld/>aj kalikoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        hungara tri<tld/>o fiksita al la kajuto
        <fnt>
          <bib>Monato</bib>
          <aut>Jozefo Horv&aacute;th</aut>
          <vrk><url ref="https://www.monato.be/2006/009060.php">Tra infanaj okuloj: memoroj pri la eventoj de '56</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Trikolore</trd>
</drv>


