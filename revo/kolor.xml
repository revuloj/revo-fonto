          <fnt>
            <bib>Monato</bib>
            <aut>Franz-Georg R&ouml;ssler</aut>
            <vrk><url ref="&Monato;/2013/010812.php">Po&scirc;to
            perdita</url></vrk>
            <lok>2013</lok>
          </fnt>.
        </ekz>
      </dif>     
    </subsnc>
    <subsnc mrk="kolor.0igisto.KON">
      <uzo tip="fak">KON</uzo>
