<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kolor</rad>/o
</kap>
<drv mrk="kolor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kolor.0o.propre">
    <dif>
      Aparta impreso farita sur la okulo de la diversaj
      elementoj de lumradio resenditaj de iu korpo sendepende
      de &gcirc;ia formo:
      <ekz>
        la sep <tld/>oj de la &ccirc;ielarko;
      </ekz>
      <ekz>
        la <tld/>o de vesto, haroj, kartoj, okuloj, ha&ubreve;to;
      </ekz>
      <ekz>
        <ind>kiam nokto vualas, &ccirc;iuj <tld/>oj egalas</ind>
        <trd lng="ca">de nit tos els gats s&oacute;n negres</trd>
        <trd lng="fr">la nuit, tous les <ind>chat</ind>s sont gris</trd>
        <trd lng="pl">noc&aogonek; wszystkie koty s&aogonek; czarne
        </trd>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        sento de feli&ccirc;eco, kiu montras al ili &ccirc;ion kun
        la plej agrablaj <tld/>oj
        <klr>(&scirc;ajno, aspekto)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        homoj de &ccirc;iuj politikaj <tld/>oj
        <klr>(opinioj, partioj)</klr>;
      </ekz>
      <ekz>
        roz<tld/>a;
      </ekz>
      <ekz>
        <ind>&scirc;an&gcirc;<tld/>a</ind>;
        <trd lng="ca">tornassolat</trd>
        <trd lng="fr">chatoyant</trd>
        <trd lng="nl">kleurrijk</trd>
      </ekz>
      <ekz>
        <ind>sang<tld/>a</ind>;
        <trd lng="fr">couleur <ind>sang</ind></trd>
        <trd lng="nl">bloedrood</trd>
        <trd lng="pl">krwistoczerwony</trd>
      </ekz>
      <ekz>
        miks<tld/>a<fnt>Z</fnt> <klr>(divers<tld/>a)</klr>;
      </ekz>
      <ekz>
        <ind>unu<tld/>a</ind>;
        <trd lng="ca">unicolor</trd>
        <trd lng="fr">monochrome</trd>
        <trdgrp lng="nl">
          <trd>eenkleurig</trd>,
          <trd>monochroom</trd>
        </trdgrp>
        <trdgrp lng="pl">
          <trd>jednobarwny</trd>,
          <trd>monochromatyczny</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind>du<tld/>a</ind>;
        <trd lng="ca">bicolor</trd>
        <trd lng="fr">bicolore</trd>
        <trd lng="nl">tweekleurig</trd>
        <trd lng="pl">dwubarwny</trd>
      </ekz>
      <ekz>
        <ind>tri<tld/>a</ind>...
        <trdgrp lng="ca">
          <trd>tricolor</trd>,
          <trd>tricrom&agrave;tic</trd>
        </trdgrp>
        <trd lng="fr">tricolore</trd>
        <trd lng="nl">driekleurig</trd>
        <trd lng="pl">tr&oacute;jbarwny</trd>
      </ekz>
    </dif>
    <trd lng="ca">color</trd>
    <trdgrp lng="pl">
      <trd>kolor</trd>,
      <trd>barwa</trd>
    </trdgrp>
    <trd lng="ru">&c_c;&c_v;&c_je;&c_t;</trd>
  </snc>
  <snc mrk="kolor.0o.sonkoloro">
    <ref tip="dif" cel="tembr.0o">tembro</ref>
    <trd lng="ca">timbre</trd>
    <trd lng="pl">barwa g&lstroke;osu</trd>
    <trd lng="ru">&c_o;&c_k;&c_r;&c_a;&c_s;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="kolor.0o.stilo">
    <ref tip="dif" cel="stil.0o">stilo</ref>
    <ekz>
      la malofteco de la adjektivoj donas al lia lingvo
      tre kuriozan <tld/>on;
    </ekz>
    <ekz>
      la laboro de la intelektuloj donas <tld/>on kaj vivon
      al la kulturo<fnt>Z</fnt>;
    </ekz>
    <ekz>
      indiferenta, pala kaj sen<tld/>a oficiala
      parolo<fnt>Z</fnt>.
    </ekz>
    <trdgrp lng="ca">
      <trd>forma</trd>,
      <trd>estil</trd>,
      <trd>expressi&oacute;</trd>
    </trdgrp>
    <trd lng="pl">koloryt</trd>
    <trd lng="ru">&c_k;&c_o;&c_l;&c_o;&c_r;&c_i;&c_t;</trd>
  </snc>
  <snc mrk="kolor.0o.ludkarta">
    <uzo tip="fak">SPO</uzo>
    <dif>
      &Ccirc;iu el la du grupoj de la ludkartaj
      <ref tip="sub" cel="emblem.0o.karta">emblemoj</ref>,
      la ru&gcirc;a (karoo kaj kero) kaj la nigra (piko kaj trefo).
    </dif>
    <trd lng="ca">color <klr>(ex: escala de color, al
    p&ograve;quer)</klr></trd>
    <trd lng="pl">kolor</trd>
    <trd lng="ru">&c_c;&c_v;&c_je;&c_t;</trd>
  </snc>
  <trd lng="br">liv</trd>
  <trd lng="en">color</trd>
  <trdgrp lng="fr">
    <trd>couleur</trd>,
    <trd>coloris</trd>,
    <trd>teinte<klr tip="ind"> (coloris)</klr></trd>,
    <trd>tendance<klr tip="ind"> (politique)</klr></trd>,
    <trd>timbre<klr tip="ind"> (sonore)</klr></trd>,
    <trd>ton<klr tip="ind"> (coloris)</klr></trd>
  </trdgrp>
  <trd lng="nl">kleur</trd>
  <trdgrp lng="pt">
    <trd>cor</trd>,
    <trd>colorido</trd>,    
    <trd>timbre <klr tip="amb">(de voz)</klr></trd>
  </trdgrp>
  <trd lng="sv">f&auml;rg</trd>
  <trd lng="tp">kule</trd>
  <trd lng="tr">renk</trd>
</drv>

<drv mrk="kolor.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Havanta <tld/>on:
      <ekz>
        <tld/>aj bildoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">colorat</trd>
  <trd lng="cs">barevn&yacute;</trd>
  <trd lng="en">colored</trd>
  <trdgrp lng="fr">
    <trd>color&eacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gekleurd</trd>,
    <trd>kleurig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kolorowy</trd>,
    <trd>barwny</trd>
  </trdgrp>
  <trd lng="ru">&c_c;&c_v;&c_je;&c_t;&c_n;&c_o;&c_j;</trd>
  <trd lng="tp">kule</trd>
  <trdgrp lng="tr">
    <trd>renkli</trd>,
    <trd>boyal&inodot;</trd>
  </trdgrp>
</drv>

<drv mrk="kolor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Doni <tld/>on al, ka&ubreve;zi <tld/>on &ccirc;e:
      <ekz>
        la grenoj flave <tld/>as la kampon;
      </ekz>
      <ekz>
        la suno <tld/>as la nubojn;
      </ekz>
      <ekz>
        la tempo grize <tld/>is liajn harojn;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la akompananta muziko <tld/>as la tutan dialogon.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">colorar</trd>
  <trd lng="en">color</trd>
  <trd lng="fr">colorer</trd>
  <trdgrp lng="nl">
    <trd>kleuren</trd>,
    <trd>verven</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kolorowa&cacute;</trd>,
    <trd>koloryzowa&cacute;</trd>,
    <trd>barwi&cacute;</trd>,
    <trd>ubarwia&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_k;&c_r;&c_a;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_k;&c_r;&c_a;&c_s;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">f&auml;rga</trd>
  <trd lng="tp">kule</trd>
  <trdgrp lng="tr">
    <trd>boyamak</trd>,
    <trd>renk vermek</trd>
  </trdgrp>
</drv>

<drv mrk="kolor.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Sur&scirc;miri ion (desegnon, gravura&jcirc;on, paperon,
      tola&jcirc;on...) per <tld/>iloj:
      <ekz>
        &scirc;i <tld/>igis sian viza&gcirc;on kaj vestis sin kiel
        maljuna butikistino
        <fnt><bib>ElektFab</bib>, <lok>Ne&gcirc;ulino</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">liva&ntilde;</trd>
  <trd lng="ca">acolorir</trd>
  <trd lng="en">color</trd>
  <trd lng="fr">colorier</trd>
  <trdgrp lng="nl">
    <trd>kleuren</trd>,
    <trd>verven</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kolorowa&cacute;</trd>,
    <trd>barwi&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_a;&c_s;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_k;&c_r;&c_a;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_k;&c_r;&c_a;&c_s;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">m&aring;la</trd>
  <trdgrp lng="tr">
    <trd>renklendirmek</trd>,
    <trd>boyamak</trd>
  </trdgrp>
</drv>

<drv mrk="kolor.0igilo">
  <kap><tld/>igilo</kap>
  <snc>
    <ref tip="dif" cel="farb.0o">farbo</ref>
    <ekz>
      desegna&jcirc;oj, faritaj sur papero per <tld/>igiloj
      <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 6a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="ca">
    <trd>pintura</trd>,
    <trd>colorant</trd>
  </trdgrp>
  <trd lng="de">Farbe</trd>
  <trd lng="en">paint</trd>
  <trdgrp lng="fr">
    <trd>peinture<klr tip="ind"> (produit &agrave; base de
    pigments)</klr></trd>,
    <trd>couleur<klr tip="ind"> (peinture)</klr></trd>
  </trdgrp>
  <trd lng="nl">verf</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_a;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_s;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="tr">boya</trd>
</drv>

<drv mrk="kolor.0igisto">
  <kap><tld/>igisto</kap>
  <snc>
    <dif>
      Homo, kies metio estas sur&scirc;miri desegna&jcirc;ojn,
      manuskriptojn ktp per <tld/>iloj:
      <ekz>
        la <tld/>igisto havas fingrojn, kiuj malbonodoras
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 1a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="ca">colorista</trd>
  <trd lng="fr">coloriste</trd>
  <trdgrp lng="tr">
    <trd>renk sanat&ccedil;&inodot;s&inodot;</trd>,
    <trd>renk&ccedil;i</trd>
  </trdgrp>
  </snc>
  <snc mrk="kolor.0igisto.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Homo, kies metio estas <tld/>ilgi &scirc;tofon, drapon.
    </dif>
    <trd lng="ca">tintorer</trd>
  <trd lng="pl">farbiarz</trd>
  </snc>
    <trd lng="nl">verver</trd>
</drv>

<drv mrk="kolor.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <ref tip="dif" cel="farb.0o">farbo</ref>
    <ekz>
      son&gcirc;oj pentris pejza&gcirc;on per <tld/>iloj el smeraldoj,
      ar&gcirc;ento, rubenoj
      <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 3a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="ca">pintura</trd>
  <trd lng="de">Farbe</trd>
  <trd lng="en">paint</trd>
  <trdgrp lng="fr">
    <trd>peinture<klr tip="ind"> (produit &agrave; base de
    pigments)</klr></trd>,
    <trd>couleur<klr tip="ind"> (peinture)</klr></trd>
  </trdgrp>
  <trd lng="nl">verf</trd>
  <trdgrp lng="pl">
    <trd>barwnik</trd>,
    <trd>pigment</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_a;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_s;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="tr">boya</trd>
</drv>

<drv mrk="kolor.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Pentristo, kiu emas reprodukti la <tld/>on pli ol la
      liniojn de la objektoj.
    </dif>
  </snc>
  <trd lng="br">liver</trd>
  <trd lng="ca">colorista</trd>
  <trd lng="en">colorist</trd>
  <trd lng="fr">coloriste</trd>
  <trd lng="nl">colorist</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_o;&c_r;&c_i;&c_s;&c_t;</trd>
</drv>

<drv mrk="kolor.divers0a">
  <kap>divers<tld/>a</kap>
  <snc mrk="kolor.divers0a.KOMUNE">
    <dif>
      Havanta plurajn malsamajn <tld/>ojn:
      <ekz>
        divers<tld/>aj papilioj
        <fnt><bib>Fab1</bib>, <lok>Elinjo-fingreto</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>policrom</trd>,
    <trd>virolat</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>multicolore</trd>,
    <trd>polychrome</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>veelkleurig</trd>,
    <trd>polychroom</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wielobarwny</trd>,
    <trd>kolorowy</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>&ccedil;ok renkli</trd>,
    <trd>reng&acirc;renk</trd>
  </trdgrp>
</drv>

<drv mrk="kolor.dom0igisto">
  <kap>dom<tld/>igisto</kap>
  <snc>
    <ref tip="dif" cel="farb.0isto">farbisto</ref>
  </snc>
  <trd lng="ca">pintor <klr>(de parets)</klr></trd>
  <trd lng="en">housepainter</trd>
  <trdgrp lng="fr">
    <trd>peintre en b&acirc;timent</trd>
  </trdgrp>
  <trd lng="nl">huisschilder</trd>
  <trd lng="pl">malarz<klr> pokojowy</klr></trd>
  <trd lng="ru">&c_m;&c_a;&c_l;&c_ja;&c_r;</trd>
  <trd lng="tr">boyac&inodot;</trd>
</drv>

<drv mrk="kolor.mult0a">
  <kap>mult<tld/>a</kap>
  <snc mrk="kolor.mult0a.KOMUNE">
    <dif>
      Havanta multajn kontrastajn kolorojn:
      <ekz>
        la duonpatrino <klr>...</klr> montris la&ccirc;on plektitan el
        mult<tld/>a silko
        <fnt><bib>ElektFab</bib>, <lok>Ne&gcirc;ulino</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">multicolor</trd>
  <trdgrp lng="fr">
    <trd>chamarr&eacute;</trd>,
    <trd>multicolore</trd>,
  </trdgrp>
   <trdgrp lng="nl">
    <trd>veelkleurig</trd>,
    <trd>polychroom</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wielobarwny</trd>,
    <trd>kolorowy</trd>
  </trdgrp>
</drv>

<drv mrk="kolor.mur0igisto">
  <kap>mur<tld/>igisto</kap>
  <snc>
    <ref tip="dif" cel="farb.0isto">farbisto</ref>
  </snc>
  <trd lng="ca">pintor (de parets)</trd>
  <trd lng="en">housepainter</trd>
  <trdgrp lng="fr">
    <trd>peintre en b&acirc;timent</trd>
  </trdgrp>
  <trd lng="nl">huisschilder</trd>
  <trd lng="pl">malarz<klr> pokojowy</klr></trd>
  <trd lng="ru">&c_m;&c_a;&c_l;&c_ja;&c_r;</trd>
</drv>

<drv mrk="kolor.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="kolor.sen0a.KOMUNE">
    <dif>
      Ne havanta koloron, tio estas a&ubreve; blanka, griza, nigra,
      a&ubreve; tute diafana:
      <ekz>
        maldika sen<tld/>a hararo
        <fnt><bib>Kiso</bib>, <lok>&Aring;ge</lok></fnt>;
      </ekz>
      <ekz>
        sen<tld/>a lako
        <fnt>
        <aut>M. Belosevic</aut>,
        <vrk>-</vrk>,
        <lok>soc.culture.esperanto, 1997-03-28</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">incolor</trd>
  <trd lng="fr">incolore</trd>
  <trd lng="nl">kleurloos</trd>
  <trd lng="pl">bezbarwny</trd>
</drv>

<drv mrk="kolor.son0o">
  <kap>son<tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <ref tip="dif" cel="tembr.0o">tembro</ref>
  </snc>
  <trd lng="ca">timbre</trd>
  <trdgrp lng="fr">
    <trd>couleur musicale</trd>,
    <trd>timbre<klr tip="ind"> (sonore)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>muziekkleur</trd>,
    <trd>timbre</trd>
  </trdgrp>
  <trd lng="pl">barwa g&lstroke;osu</trd>
  <trd lng="ru">&c_o;&c_k;&c_r;&c_a;&c_s;&c_k;&c_a;</trd>
</drv>
</art>
