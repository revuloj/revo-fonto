      a&ubreve; tute diafana:
      <ekz>
        maldika sen<tld/>a hararo
        <fnt><bib>Kiso</bib>, <lok>&Aring;ge</lok></fnt>;
      </ekz>
      <ekz>
        sen<tld/>a lako
        <fnt>
        <aut>M. Belosevic</aut>,
        <vrk>-</vrk>,
        <lok>soc.culture.esperanto, 1997-03-28</lok></fnt>;
      </ekz>
      <ekz>
        la sen<tld/>i&gcirc;intaj pupiloj de la maljunulino ne malseki&gcirc;is per
        larmo
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_s;&c_k;&c_o;&c_l;&c_je;&c_r;&c_n;&c_y;</trd>
  <trd lng="bg">&c_b;&c_je;&c_z;&c_c;&c_v;&c_je;&c_t;&c_je;&c_n;</trd>
  <trd lng="ca">incolor</trd>
  <trd lng="cs">bezbarv&yacute;</trd>
  <trd lng="de">farblos</trd>
  <trd lng="en">colorless</trd>
  <trd lng="fr">incolore</trd>
  <trdgrp lng="id">
    <trd><ind>tanpa</ind> warna</trd>,
    <trd>takberwarna</trd>,
    <trd>tanwarna</trd>
  </trdgrp>
  <trd lng="nl">kleurloos</trd>
  <trd lng="pl">bezbarwny</trd>
  <trd lng="sk">bezfarebn&yacute;</trd>
  <trd lng="uk">&c_b;&c_je;&c_z;&c_b;&c_a;&c_r;&c_v;&#x43D;&c_i;&c_j;</trd>
</drv>

<drv mrk="kolor.son0o">
  <kap>son<tld/>o</kap>
