<drv mrk="kolor.sxangx0a">
  <kap>&scirc;an&gcirc;<tld/>a</kap>
  <snc mrk="kolor.sxangx0a.KOL">
    <dif>
      Kies <tld/>o facile &scirc;an&gcirc;i&gcirc;as la&ubreve; rebriloj
      de la lumo a&ubreve; pro alia ka&ubreve;zo:
      <ekz>
        komenci&gcirc;is traveturado de la a&ubreve;gustanoj &mdash;
        vicaro impona, brila, &scirc;an&gcirc;<tld/>a, kiel serpento
        <fnt>
          <aut>H. Sienkiewicz, trad. Lydia Zamenhof</aut>
          <vrk>Quo vadis?</vrk>
          <lok>1933</lok>
        </fnt>.
      </ekz>     
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_l;&c_ib;&c_v;&c_ib;&c_s;&c_t;&c_y; <klr>(&c_p;&c_r;&c_a; &c_k;&c_o;&c_l;&c_je;&c_r;)</klr></trd>
  <trd lng="ca">tornassolat</trd>
  <trd lng="fr">chatoyant</trd>
  <trd lng="nl">kleurrijk</trd>
  <trd lng="uk">&#x43F;&c_je;&c_r;&c_je;&#x43B;&c_i;&c_v;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_j;</trd>
</drv>

<drv mrk="kolor.tri0eto">
  <kap>tri<tld/>eto</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="kolor.tri0eto.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="dif" cel="viol.0ego.HOR">Violego</ref>:
      <ekz>
        ili prenis kun si anka&ubreve; la malestimatan kalendulon kaj la
        sova&gcirc;an tri<tld/>eton
        <fnt>
          <bib>Fab2</bib>
          <lok>An&gcirc;elo</lok>
        </fnt>;
      </ekz>
      <ekz>
        alpaj edelvejsoj kaj moder-klimataj tri<tld/>etoj kune
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj'</aut>
          <vrk><url ref="&Monato;/2015/011315.php">La
          islanda: revo de ne plu juna lernemulo</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#8230;]</klr> veturis sur la tri<tld/>etoj
        &ccirc;irka&ubreve; la monumento 
        <fnt>
          <aut>A. Gregor, trad. J. Chvosta</aut>,
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr46/auto.html">
          A&ubreve;to-ekskurso</url></vrk>,
          <lok>Nica Literatura Revuo, 1959-07 &gcirc;is 08 (4/6), p.
          294-298a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_a;&c_t;&c_k;&c_ib; <klr>(&c_k;&c_v;&c_je;&c_t;&c_k;&c_a;)</klr></trd>
  <trd lng="bg">&c_t;&c_je;&c_m;&c_je;&c_n;&c_u;&c_g;&c_i;</trd>
  <trdgrp lng="cs">
    <trd>mace&scaron;ka</trd>,
    <trd>mace&scaron;ka trojbarevn&aacute;</trd>
  </trdgrp>
  <trd lng="de">Stiefm&uuml;tterchen</trd>
  <trd lng="en">pansy</trd>
  <trd lng="fa">&a_b;&a_n;&a_f;&a_sh;&a_h;&a_hamza_sure;
  &a_s;&a_h;&zwnj;&a_r;&a_n;&f_g;</trd>
  <trd lng="fr">pens&eacute;e<klr> (botanique)</klr></trd>
  <trd lng="hu">&aacute;rv&aacute;cska</trd>
  <trd lng="nl">driekleurig <ind>viooltje</ind></trd>
  <trdgrp lng="pl">
    <trd>bratek</trd>,
    <trd>fio&lstroke;ek tr&oacute;jbarwn</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_jo;&c_h;&c_c;&c_v;&c_je;&c_t;&c_n;&c_a;&c_ja;
    <ind>&c_f;&c_i;&c_a;&c_l;&c_k;&c_a;</ind></trd>,
    <trd>&c_a;&c_n;&c_ju;&c_t;&c_i;&c_n;&c_y;
    &c_g;&c_l;&c_a;&c_z;&c_k;&c_i;</trd>
  </trdgrp>
  <trd lng="sk">sir&ocirc;tka</trd>
  <trd lng="sv">styvmorsviol</trd>
  <trd lng="uk">&c_b;&c_r;&c_a;&c_t;&#x43A;&c_i;</trd>
</drv>

<drv mrk="kolor.tri0o">
  <kap>
    tri<tld/>o
  </kap>
  <snc mrk="kolor.tri0o.flago">
    <dif>
      Flago tri<tld/>a:
      <ekz>
        tri<tld/>oj kun belaj viol<tld/>aj kalikoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        hungara tri<tld/>o fiksita al la kajuto
        <fnt>
          <bib>Monato</bib>
          <aut>Jozefo Horv&aacute;th</aut>
          <vrk><url ref="&Monato;/2006/009060.php">Tra
          infanaj okuloj: memoroj pri la eventoj de '56</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_y;&c_k;&c_a;&c_l;&c_jo;&c_r;</trd>
  <trd lng="de">Trikolore</trd>
  <trdgrp lng="en">
    <trd>tricolour</trd>,
    <trd>tricolor</trd>
  </trdgrp>
</drv>

<drv mrk="kolor.vaks0a">
  <kap>
    vaks<tld/>a
  </kap>
  <snc>
    <dif>
      <ref tip="dif" cel="pal.0a.vizagxe">Pala</ref>, sensanga:
      <ekz>
        sur liaj vaks<tld/>aj vangoj ekbrulis ru&gcirc;aj makuloj
        <fnt>
          <bib>KKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        blanke uniformitaj homoj, kun vaks<tld/>aj viza&gcirc;oj
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        la komandanto nur retrovis malfeli&ccirc;an knabinon palan, sensangan, vakso<tld/>an,
        pli bezonantan flegistinon ol amanton
        <fnt>
          <bib>KPr</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kolor.sen0a">senkolora</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_l;&c_je;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_ja;&c_s;&c_k;&c_r;&c_o;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_v;&c_a;&c_s;&c_k;&c_o;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>w&auml;chsern</trd>,
    <trd>bleich</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&#x43E;&c_shch;&c_a;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_b;&#x43B;&c_ib;&c_d;&#x43E;-&c_zh;&#x43E;&c_v;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
</drv>


