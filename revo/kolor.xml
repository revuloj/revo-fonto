  <trd lng="br">liv</trd>
  <trd lng="bs">u boji</trd>
  <trd lng="co">culore</trd>
  <trdgrp lng="cs">
    <trd>barva</trd>,
    <trd>instrukce pro nastaven&iacute; barevn&eacute;ho
    v&yacute;stupu</trd>
  </trdgrp>
  <trd lng="cy">lliw</trd>
  <trd lng="da">farve</trd>
  <trd lng="de">Farbe</trd>
  <trdgrp lng="en">
    <trd>color</trd>,
    <trd>colour</trd>
  </trdgrp>
  <trd lng="es">color</trd>
  <trd lng="et">v&auml;rv</trd>
  <trd lng="eu">kolorea</trd>
  <trdgrp lng="fr">
    <trd>couleur</trd>,
    <trd>coloris</trd>,
    <trd>teinte<klr tip="ind"> (coloris)</klr></trd>,
    <trd>ton<klr tip="ind"> (coloris)</klr></trd>
  </trdgrp>
  <trd lng="fy">kleur</trd>
  <trd lng="ga">dath</trd>
