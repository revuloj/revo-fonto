<drv mrk="kolor.0a">
  <kap><tld/>a</kap>
  <snc mrk="kolor.0a.havanta">
    <dif>
      Havanta <tld/>on:
      <ekz>
        <tld/>aj bildoj;
      </ekz>
      <ekz>
        briko<tld/>a muro
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        bel<tld/>aj floroj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        rozo<tld/>a bu&scirc;o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        pajlo<tld/>a
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        ar&gcirc;ent<tld/>aj haroj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        cindro<tld/>a vesto
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        pentra&jcirc;oj flam<tld/>aj
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>sang<tld/>a</ind>;
        <trd lng="fr">couleur <ind>sang</ind></trd>
        <trd lng="nl">bloedrood</trd>
        <trd lng="pl">krwistoczerwony</trd>
      </ekz>
      <ekz>
        <ind>unu<tld/>a</ind>;
        <trd lng="ca">unicolor</trd>
        <trd lng="de">einfarbig</trd>
        <trd lng="fr">monochrome</trd>
        <trd lng="id">monokrom</trd>
        <trdgrp lng="nl">
          <trd>eenkleurig</trd>,
          <trd>monochroom</trd>
        </trdgrp>
        <trdgrp lng="pl">
          <trd>jednobarwny</trd>,
          <trd>monochromatyczny</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind>du<tld/>a</ind>;
        <trd lng="ca">bicolor</trd>
        <trd lng="de">zweifarbig</trd>
        <trd lng="fr">bicolore</trd>
        <trd lng="id">dwiwarna</trd>
        <trd lng="nl">tweekleurig</trd>
        <trd lng="pl">dwubarwny</trd>
      </ekz>
      <ekz>
        <ind>tri<tld/>a</ind>.
        <trdgrp lng="ca">
          <trd>tricolor</trd>,
          <trd>tricrom&agrave;tic</trd>
        </trdgrp>
        <trd lng="de">dreifarbig</trd>
        <trd lng="fr">tricolore</trd>
        <trd lng="id">triwarna</trd>
        <trd lng="nl">driekleurig</trd>
        <trd lng="pl">tr&oacute;jbarwny</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_ja;&c_r;&c_o;&c_v;&c_y;</trd>
  <trdgrp lng="bg">
    <trd>&c_o;&c_c;&c_v;&c_je;&c_t;&c_je;&c_n;</trd>,
    <trd>&c_c;&c_v;&c_je;&c_t;&c_n;&c_o;&c_k;&c_o;&c_zh;</trd>
  </trdgrp>
  <trd lng="ca">colorat</trd>
  <trdgrp lng="cs">
    <trd>barevn&yacute;</trd>,
    <trd>barvit&yacute;</trd>
  </trdgrp>
  <trd lng="de">farbig</trd>
  <trdgrp lng="en">
    <trd>colored</trd>,
    <trd>coloured</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>color&eacute;</trd>
  </trdgrp>
  <trd lng="id">ber<ind>warna</ind></trd>
  <trdgrp lng="nl">
    <trd>gekleurd</trd>,
    <trd>kleurig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kolorowy</trd>,
    <trd>barwny</trd>
  </trdgrp>
  <trd lng="ru">&c_c;&c_v;&c_je;&c_t;&c_n;&c_o;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>farbist&yacute;</trd>,
    <trd>pestrofarebn&yacute;</trd>,
    <trd>pestr&yacute;</trd>,
    <trd>viacfarebn&yacute;</trd>
  </trdgrp>
  <trd lng="tok">kule</trd>
  <trdgrp lng="tr">
    <trd>renkli</trd>,
    <trd>boyal&inodot;</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&#x43E;&#x43B;&#x44C;&#x43E;&c_r;&#x43E;&c_v;&c_i;&c_j;</trd>
</drv>

<drv mrk="kolor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kolor.0i.doni">
    <dif>
      Doni <tld/>on al, ka&ubreve;zi <tld/>on &ccirc;e:
      <ekz>
        la grenoj flave <tld/>as la kampon;
      </ekz>
      <ekz>
        la suno <tld/>as la nubojn;
      </ekz>
      <ekz>
        la tempo grize <tld/>is liajn harojn;
      </ekz>
      <ekz>
        sur la portretoj el Kembri&gcirc;o espera &gcirc;ojo 
        <tld/>as la zorgemon en okuloj kaj sur
        lipoj de la Majstro
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la akompananta muziko <tld/>as la tutan dialogon.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_r;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_o;&c_c;&c_v;&c_je;&c_t;&c_ja;&c_v;&c_a;&c_m;</trd>
  <trd lng="ca">colorar</trd>
  <trd lng="cs">kolorovat</trd>
  <trd lng="de">f&auml;rben</trd>
  <trdgrp lng="en">
    <trd>color</trd>,
    <trd>colour</trd>
  </trdgrp>
  <trd lng="fr">colorer</trd>
  <trd lng="id">me<ind>warna</ind>i</trd>
  <trdgrp lng="nl">
    <trd>kleuren</trd>,
    <trd>verven</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kolorowa&cacute;</trd>,
    <trd>koloryzowa&cacute;</trd>,
    <trd>barwi&cacute;</trd>,
    <trd>ubarwia&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_k;&c_r;&c_a;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_k;&c_r;&c_a;&c_s;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">kolorova&tcaron;</trd>
  <trd lng="sv">f&auml;rga</trd>
  <trd lng="tok">kule</trd>
  <trdgrp lng="tr">
    <trd>boyamak</trd>,
    <trd>renk vermek</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&c_r;&c_v;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_b;&c_a;&c_r;&c_v;&#x43B;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43D;&c_a;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &c_z;&c_a;&c_b;&c_a;&c_r;&c_v;&#x43B;&c_je;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="kolor.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="kolor.0igi.sursxmiri">
    <dif>
      Sur&scirc;miri ion (desegnon, gravura&jcirc;on, paperon,
      tola&jcirc;on...) per <tld/>iloj:
      <ekz>
        vir&scirc;afaj feloj ru&gcirc;e <tld/>igitaj
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 25:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>igis sian viza&gcirc;on kaj vestis sin kiel
        maljuna butikistino
