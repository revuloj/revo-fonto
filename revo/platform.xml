<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>platform</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="platform.0o">
  <kap><tld/>o</kap>
  <snc mrk="platform.0o.FER">
    <uzo tip="fak">FER</uzo>
    <dif>
      Plata starejo fine de <ref tip="malprt"
      cel="vagon.0o">vagono</ref>,
      buso a&ubreve; alia komuna veturilo:
      <ekz>
        mi pa&scirc;as post <tld/>o, ruli&gcirc;anta sur la reloj
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ja;&c_t;&c_f;&c_o;&c_r;&c_m;&c_a; <klr
  tip="amb">(&c_v;&c_a;&c_g;&c_o;&c_n;&c_a;)</klr></trd>
  <trd lng="ca">plataforma</trd>
  <trd lng="es">plataforma<klr> (de un veh&iacute;culo)</klr></trd>
  <trd lng="fr">plate-forme<klr> (de v&eacute;hicule)</klr></trd>
  <trd lng="hu">peron<klr tip="amb"> (j&aacute;rm&udblac;v&ouml;n)</klr></trd>
  <trd lng="nl">platform<klr tip="amb"> (v.voertuig)</klr></trd>
  <trd lng="pt">plataforma</trd>
  <trdgrp lng="sk">
    <trd>platforma</trd>,
    <trd>východisko</trd>,
    <trd>základňa</trd>
  </trdgrp>
</drv>
</art>

