<kap>
  <rad>akumulator</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="akumul1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ELE</uzo>
  <fnt><bib>ZR</bib></fnt>
  <snc>
    <dif>
      Aparato por amasigi provizon da elektro kaj &gcirc;in redoni
      la&ubreve;vole:
      <ekz>
        elektra lumo provizata per sunpanelo kaj
        akumulatoro<fnt><aut>Saliko</aut>: <vrk>&Gcirc;ojoj kaj
        &gcirc;uoj de finna
        somero</vrk>, <bib>Monato</bib><lok>jaro 2000a, numero 12a, p.
        12a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pil.0o">pilo</ref>,
      <ref cel="bateri.0o.ELE">baterio</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_u;&c_m;&c_u;&c_l;&c_ja;&c_t;&c_a;&c_r;</trd>
  <trd lng="ca">acumulador <klr>(d'energia)</klr></trd>
  <trd lng="cs">akumul&#xE1;tor</trd>
  <trdgrp lng="de">
    <trd>Akku</trd>,
    <trd>Akkumulator</trd>
  </trdgrp>
  <trdgrp lng="en">
  	<trd>accumulator</trd>,
  	<trd>rechargeable battery</trd>
  </trdgrp>
  <trd lng="es">acumulador</trd><trd lng="es">acumulador</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_n;&a_b;&a_A;&a_r;&a_h;</trd>,
    <trd>&a_b;&a_A;&a_t;&a_r;&f_y; &a_A;&a_n;&a_b;&a_A;&a_r;&a_sh;&f_y;</trd>,
    <trd>&a_A_madda;&f_k;&a_w;&a_m;&a_w;&a_l;&a_A;&a_t;&a_w;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">accumulateur</trd>
  <trd lng="he">&#x5DE;&#x5E6;&#x5D1;&#x5E8;</trd>
  <trd lng="hu">akkumul&aacute;tor</trd>
  <trd lng="ja">&#x84C4;&#x96FB;&#x6C60;  [<ind>&#x3061;&#x304F;&#x3067;&#x3093;&#x3061;</ind>]</trd>
  <trdgrp lng="nl">
    <trd>accu</trd>,
    <trd>accumulator</trd>
  </trdgrp>
  <trd lng="pl">akumulator<klr> elektryczny</klr></trd>
  <trd lng="pt">acumulador</trd>
  <trd lng="ru">&c_a;&c_k;&c_k;&c_u;&c_m;&c_u;&c_l;&c_ja;&c_t;&c_o;&c_r;</trd>
  <trd lng="sk">akumul&aacute;tor</trd>
  <trd lng="uk">&#x430;&#x43A;&#x443;&#x43C;&#x443;&#x43B;&#x44F;&#x442;&#x43E;&#x440;</trd>
</drv>
</art>
