<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bosx.xml,v 1.3 2023/03/06 08:35:57 revo Exp $">
<kap>
  <rad>bo&scirc;</rad>/o
</kap>
<drv mrk="bosx.0o">
  <kap><tld/>o</kap>
  <snc mrk="bosx.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Tropika kamparo kun malabunda vivo kaj kreska&jcirc;aro el
      <ref cel="vepr.0ejo.GEOG">veprejoj</ref> a&ubreve; maldensaj
      arbaroj:
      <ekz>
        li estis tute sensperta pri la a&ubreve;stralia <tld/>o
        <fnt><aut>T. Steele</aut>, <vrk>Flugi kun kakatuoj</vrk>, 
        <lok>2010</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="makis.0o.GEOG">makiso</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_sh;</trd>
  <trdgrp lng="fr">
    <trd>brousse</trd>,
    <trd>bush<klr> (australien etc.)</klr></trd>
  </trdgrp>
  <trd lng="pl">busz</trd>
</drv>

<drv mrk="bosx.0ano">
  <kap><tld/>ano</kap>
  <snc mrk="bosx.0ano.GEOG">
    <dif>
      Lo&gcirc;anto de <tld/>a regiono:
      <ekz>
        <tld/>anoj amas rajdi
        <fnt>
          <aut>A. B. Paterson, trad. A. Towsey</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Poezio/negxriverou.html">
          La vir' el Ne&gcirc;rivero</url></vrk>,
          <lok>en: A&ubreve;stralia Antologio, 1988</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_sh;&c_m;&c_e;&c_n;</trd>
  <trd lng="fr">broussard</trd>
  <trd lng="pl">buszmen</trd>
</drv>
</art>

