<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>varb</rad>/i
</kap>
<drv mrk="varb.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="varb.0i.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Fari per siaj instigoj, ke iu soldati&gcirc;u:
      <ekz>
        la aventuristojn ili <tld/>is en la armeon<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>anwerben</trd>,
      <trd>rekrutieren</trd>
    </trdgrp>
    <trd lng="it">reclutare</trd>
    <trd lng="nl">rekruteren</trd>
  </snc>
  <snc mrk="varb.0i.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Fari per siaj instigoj, ke iu ani&gcirc;u al grupo, partio, doktrino:
      <ekz>
        estas pli facile <tld/>i iun al Esperanto ol fari ke la
        <tld/>ito
        i&gcirc;u vera praktikanto de la lingvo;
      </ekz>
      <ekz>
        <tld/>i samideanojn<fnt>B</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="konver3.0i.FIG.al.opinio">konverti</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>werben</trd>,
      <trd>anwerben</trd>,
      <trd>gewinnen</trd>
    </trdgrp>
    <trd lng="it">affiliare</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_e;&c_r;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_a;&c_b;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_v;&c_a;&c_b;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_c;&c_ja;&c_g;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">tuta</trd>
  <trdgrp lng="ca">
    <trd>reclutar</trd>,
    <trd>allistar</trd>,
    <trd>enrolar</trd>
  </trdgrp>
  <trd lng="en">enlist</trd>
  <trd lng="es">reclutar</trd>
  <trdgrp lng="fr">
    <trd>recruter</trd>
  </trdgrp>
  <trd lng="he">&lamed;<ind>&gimel;&yod;&yod;&samekh;</ind></trd>
  <trdgrp lng="hu">
    <trd>toboroz</trd>,
    <trd>verbuv&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ronselen</trd>,
    <trd>werven</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>alistar</trd>,
    <trd>recrutar</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_je;&c_r;&c_b;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd><klr>(z)</klr>verbovať</trd>,
    <trd>robiť nábor</trd>,
    <trd>verbova&tcaron;</trd>
  </trdgrp>
</drv>
</art>
