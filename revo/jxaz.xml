<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&jcirc;az</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="jxaz.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzika stilo iniciatita de usonaj negroj fine de la 19a jarcento,
      sinteza de afrikaj kaj e&ubreve;ropaj influoj,
      kun sinkopa ritmo, propraj tonaloj, improvizemo.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_zh;&c_a;&c_z;</trd>
  <trd lng="de">Jazz</trd>
  <trd lng="es">jazz</trd>
  <trdgrp lng="fr">
    <trd>jazz</trd>
  </trdgrp>
  <trd lng="hu">dzsessz</trd>
  <trd lng="ru">&c_d;&c_zh;&c_a;&c_z;</trd>
</drv>

<drv mrk="jxaz.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="muzik.0isto">Muzikisto</ref>, kiu praktikas
      <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_zh;&c_a;&c_z;&c_m;&c_e;&c_n;</trd>
  <trdgrp lng="de">
    <trd>Jazz-Musiker</trd>,
    <trd>Jazzer</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>jazz-man</trd>
  </trdgrp>
  <trd lng="hu">dzsesszmuzsikus</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_zh;&c_a;&c_z;&c_m;&c_je;&c_n;</trd>,
    <trd>&c_d;&c_zh;&c_a;&c_z;&c_i;&c_s;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="jxaz.novorleana0o">
  <kap>Nov-Orleana <tld/>o</kap> <fnt><bib>RokPop</bib></fnt>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Pra&jcirc;azo kreita &ccirc;irka&ubreve; 1900 en Nov-Orleano en Usono, ludata de tri a&ubreve;
      kvar blovinstrumentoj kaj ritma sekcio.
    </dif>
  </snc>
  <trd lng="de">New-Orleans-Jazz</trd>
  <trdgrp lng="fr">
    <trd>jazz <ind>New Orleans</ind></trd>,
    <trd>jazz <ind>Nouvelle-Orl&eacute;ans</ind></trd>
  </trdgrp>
  <trd lng="hu">New Orleans-i dzsessz</trd>
</drv>
</art>
