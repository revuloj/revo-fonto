<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: balbo.xml,v 1.10 2023/08/11 09:25:20 revo Exp $">
<kap>
  <rad>balbo</rad>/o
</kap>

<drv mrk="balbo.0o">
  <kap><tld/>o</kap>
  <snc mrk="balbo.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="lst" cel="unu.mon0o.EKON"
      lst="voko:monunuoj">Monunuo</ref> de <ref tip="malprt" cel="panam.P0o">Panamo</ref>,
      uzata kune kun la usona dolaro:
      <ekz>
        balboo, <klr>[&#8230;]</klr> la nomo de la mon-unuo de Panamo <klr>[&#8230;]</klr> ricevis
        tiun nomon oma&gcirc;e al hispana esploristo <tld lit="B"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=43776"
              >Panama balboo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_l;&c_mol;&c_b;&c_o;&c_a;</trd>
  <trd lng="bg">&c_b;&c_a;&c_l;&c_b;&c_o;&c_a;</trd>
  <trd lng="de">Balboa</trd>
  <trd lng="en">balboa</trd>
  <trd lng="es">balboa</trd>
  <trd lng="fr">balboa</trd>
  <trd lng="hu">balboa</trd>
  <trd lng="id">balboa</trd>
  <trd lng="pl">balboa<klr> (1 balboa = 100 centesimos)</klr></trd>
</drv>
</art>
