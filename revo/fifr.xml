<drv mrk="fifr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Mallonga <ref tip="super" cel="instru2.blov0o">blovinstrumento</ref>,
      soprana <ref tip="vid" cel="flut.transversa.0o">transversa
      fluto</ref>.
