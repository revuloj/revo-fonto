<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fifr.xml,v 1.20 2022/10/18 18:54:31 revo Exp $">
<kap>
<rad>fifr</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fifr.0o">
  <kap><tld/>o</kap>
  <snc mrk="fifr.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Mallonga <ref tip="lst" cel="instru2.blov0o"
      lst="voko:blovinstrumentoj">blovinstrumento</ref>,
      soprana <ref tip="vid" cel="flut.transversa0o">transversa
      fluto</ref>:
      <ekz>
        mi vidis regimenton, kiu mar&scirc;is la&ubreve; takto de <tld/>oj
        <fnt><aut>R. L. Stevenson, trad. A. Goodheir</aut>, 
        <vrk>Forkaptita</vrk>, <lok>Kardo, 1976</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_u;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_zh;&c_a;&c_l;&c_je;&c_j;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_mol;&c_v;&c_ib;&c_s;&c_mol;&c_c;&c_jo;&c_l;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">flaut&iacute;</trd>
  <trd lng="cs">d&rcaron;ev&#x11B;n&#xE1; p&#xED;&scaron;&tcaron;alka</trd>
  <trd lng="de">Querpfeife</trd>
  <trdgrp lng="fr">
    <trd>fifre</trd>,
    <trd>piccolo</trd>,
    <trd>picolo</trd>
  </trdgrp>
  <trd lng="hu">pikol&oacute;</trd>
  <trdgrp lng="it">
    <trd>piffero<klr> (flauto traverso)</klr></trd>,
    <trd>fiffaro</trd>
  </trdgrp>
  <trd lng="pl">piszcza&lstroke;ka</trd>
  <trdgrp lng="pt">
    <trd>p&iacute;faro</trd>,
    <trd>p&iacute;fano</trd>,
    <trd>pife</trd>,
    <trd>pifre</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_v;&c_i;&c_r;&c_je;&c_l;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>pikola</trd>,
    <trd>p&#xED;&scaron;&tcaron;ala</trd>
  </trdgrp>
  <trd lng="uk">&c_d;&c_u;&c_d;&#x43A;&c_a;</trd>
</drv>
</art>
