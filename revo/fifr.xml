<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fifr.xml,v 1.18 2015/07/04 08:37:34 revo Exp $">
<kap>
<rad>fifr</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fifr.0o">
  <kap><tld/>o</kap>
  <snc mrk="fifr.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Mallonga <ref tip="lst"
      cel="instru2.blov0o"
      lst="voko:blovinstrumentoj">blovinstrumento</ref>,
      soprana <ref tip="vid" cel="flut.transversa0o">transversa
      fluto</ref>:
      <ekz>
        mi vidis regimenton, kiu mar&scirc;is la&ubreve; takto de <tld/>oj
        <fnt><aut>R. L. Stevenson, trad. A. Goodheir</aut>, 
        <vrk>Forkaptita</vrk>, <lok>Kardo, 1976</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">flaut&iacute;</trd>
  <trd lng="cs">dřevěná píšťalka</trd>
  <trd lng="de">Querpfeife</trd>
  <trdgrp lng="fr">
    <trd>fifre</trd>,
    <trd>piccolo</trd>,
    <trd>picolo</trd>
  </trdgrp>
  <trd lng="hu">pikol&oacute;</trd>
  <trdgrp lng="it">
    <trd>piffero<klr> (flauto traverso)</klr></trd>,
    <trd>fiffaro</trd>
  </trdgrp>
  <trd lng="pl">piszcza&lstroke;ka</trd>
  <trdgrp lng="pt">
    <trd>p&iacute;faro</trd>,
    <trd>p&iacute;fano</trd>,
    <trd>pife</trd>,
    <trd>pifre</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_v;&c_i;&c_r;&c_je;&c_l;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>pikola</trd>,
    <trd>píšťala</trd>
  </trdgrp>
</drv>
</art>
