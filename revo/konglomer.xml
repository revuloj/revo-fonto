<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: konglomer.xml,v 1.10 2023/12/03 09:58:55 revo Exp $">
<kap>
  <rad>konglomer</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="konglomer.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="konglomer.0i.GEOL">
    <dif>
      <ref tip="super" cel="glu.kun0i">Kunglui</ref> en koheran, solidan tuton:
      <ekz>
        ili enbatis lignajn kelojn kaj poste per
        argilo miksita kun pajlo ilin stukis;
        tiel la fostokonstrua&jcirc;on ili <tld/>is
        <fnt>
         <aut>A. Jir&aacute;sek, trad. J. Vondrou&scaron;ek</aut>,
         <vrk><url ref="http://i-espero.info/files/elibroj/eo%20-%20jirasek,%20alois%20-%20malnovaj%20mitoj%20cxehxaj.pdf">
         Malnovaj mitoj &ccirc;e&hcirc;aj</url></vrk>,
         <lok>[1984]</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="aglome.0i">aglomeri</ref>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_v;&c_a;&c_r;&c_a;&c_c;&c_mol; <ind>&c_k;&c_a;&c_n;&c_g;&c_l;&c_ja;&c_m;&c_e;&c_r;&c_a;&c_t;</ind></trd>
  <trd lng="cs">tvo&rcaron;it konglomer&aacute;t</trd>
  <trd lng="de">konglomerieren</trd>
  <trdgrp lng="fr">
    <trd>agglom&eacute;rer</trd>,
    <trd>conglom&eacute;rer</trd>
  </trdgrp>
  <trd lng="pl">konglomerowa&cacute;</trd>
  <trd lng="sk">tvori&tcaron; konglomer&aacute;t</trd>
</drv>

<drv mrk="konglomer.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="konglomer.0ajxo.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      <ref tip="super" cel="mas.0o.kacxo">Maso</ref> el mineralaj eroj nature kuncementitaj;
      <ref tip="sin" cel="konglo.0o.MIN">konglomerato</ref>:
      <ekz>
        bedoj de <tld/>a&jcirc;oj <klr>[&#x2026;]</klr> estas multajn
        milojn da piedoj <klr>(futoj)</klr> dikaj
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 9a</lok></fnt>.
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ISS <klr>[&#x2026;]</klr> la &leftquot;esplorurbo en la spaco&rightquot; degeneris al nura riparejo
        <klr>[&#x2026;]</klr>, dek jarojn poste &gcirc;i estas nenio alia ol <tld/>a&jcirc;o de
        lo&gcirc;konteneroj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2009/009681.php">Politika sukceso, scienca fiasko</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="aglome.0ajxo">aglomera&jcirc;o</ref>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_g;&c_l;&c_ja;&c_m;&c_e;&c_r;&c_a;&c_t;</trd>
  <trd lng="ca">conglomerat</trd>
  <trd lng="de">Konglomerat</trd>
  <trd lng="en">conglomerate</trd>
  <trd lng="es">conglomerado</trd>
  <trdgrp lng="fa">
    <trd>&a_j;&a_w;&a_sh;&a_s;&a_n;&f_g;</trd>,
    <trd>&f_k;&a_damma;&a_n;&f_g;&a_kasra;&a_l;&a_damma_w;&a_m;&a_kasra;&a_r;&a_A;</trd>
  </trdgrp>
  <trd lng="fr">conglom&eacute;rat</trd>
  <trd lng="hu">konglomer&aacute;tum</trd>
  <trd lng="nl">conglomeraat</trd>
  <trdgrp lng="pl">
    <trd>konglomerat</trd>,
    <trd>zlepieniec</trd>,
    <trd>zlepek</trd>
  </trdgrp>
  <trd lng="pt">conglomerado</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_g;&c_l;&c_o;&c_m;&c_je;&c_r;&c_a;&c_t;</trd>
  <trd lng="zh">&#x96C6;&#x6599; <pr>j&#xED;li&#xE0;o</pr></trd>
</drv>
</art>

