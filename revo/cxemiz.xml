<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&ccirc;emiz</rad>/o
</kap>
<drv mrk="cxemiz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Maldika sur- a&ubreve; <ref tip="super" cel="vest.sub0o">subvesto</ref>
      kovranta la trunkon, parte a&ubreve; tute la brakojn:
      <ekz>
        la sud-tirolano iras kun malbutonumita <tld/>o kaj kun tibeta
        &ccirc;eneto &ccirc;irka&ubreve; la kolo<fnt><aut>Evgeni Georgiev</aut>:
        <vrk>Reinhold Messner - Alpisto, revanto, e&ubreve;ropa deputito</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 8a, p. 26a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="el">
    <trd>&pi;&omikron;&ypsilon;&kappa;&alfa_ton;&my;&jota;&sigma;&omikron;</trd>,
    <trd>&ypsilon;&pi;&omikron;&kappa;&alfa_ton;&my;&jota;&sigma;&omikron;</trd>
  </trdgrp>
  <trd lng="de">Hemd</trd>
  <trd lng="cs">ko&scaron;ile</trd>
  <trd lng="en">shirt</trd>
  <trd lng="es">camisa</trd>
  <trd lng="fr">chemise</trd>
  <trd lng="he">&het;&vav;&lamed;&tsadi;&he;</trd>
  <trd lng="nl">hemd</trd>
  <trd lng="pt">camisa</trd>
  <trd lng="pl">koszula</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_u;&c_b;&c_a;&c_h;&c_a;</trd>,
    <trd>&c_r;&c_u;&c_b;&c_a;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_o;&c_r;&c_o;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="tr">g&ouml;mlek</trd>
</drv>

<drv mrk="cxemiz.to0o">
  <kap>to-<tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Neformala <ref tip="super" cel="cxemiz.0o"><tld/></ref>
      kun mallongaj manikoj &gcirc;is mezo de supra brako,
      portata sub- a&ubreve; sol-veste:
      <ekz>
        li kreis T-<tld/>ojn kaj po&scirc;tkartojn<fnt><aut>John G.
        Champlin</aut>: <vrk>Kiel pentri demokration?</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 12a, p. 20a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>tee-shirt</trd>,
    <trd>t-shirt</trd>
  </trdgrp>
  <trd lng="es">camiseta</trd>
  <trdgrp lng="fr">
    <trd>tee-shirt</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>camisola</trd>,
    <trd>camiseta<klr> (bras.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_j;&c_k;&c_a;</trd>,
    <trd>&c_f;&c_u;&c_t;&c_b;&c_o;&c_l;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
