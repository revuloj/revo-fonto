<art mrk="$Id: anal.xml,v 1.6 2020/03/22 21:10:41 revo Exp $">
<kap>
  <rad>anal</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="anal.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="kronik1.0o.verko">Kroniko <sncref/></ref>:
      <ekz>
        &ccirc;u viaj mo&scirc;toj ne legis <klr>...</klr> la <tld/>ojn kaj
        historiojn
        de Anglujo <klr>...</klr><fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro
        13a, p. 92a</lok></fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="en">annal</trd>
  <trd lng="es">anal</trd>
  <trdgrp lng="fr">
    <trd>annale</trd>,
    <trd>chronique<klr> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
  <trd>anna&lstroke;</trd>,
  <trd>rocznik</trd>
  </trdgrp>
  <trd lng="pt"><klr>(&ccirc;iam uzata en la plurala formo): </klr>anais</trd>
</drv>
</art>

