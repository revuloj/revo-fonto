    <trd>agnolotti</trd>,
    <trd>tortellini</trd>,
    <trd>ravioli</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#39290;&#23376;</trd>,
    <trd><klr>tr. </klr>&#39171;&#23376;<klr> [ji&#462;ozi]</klr></trd>
