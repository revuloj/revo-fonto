<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>pelmen</rad>/oj</kap>
<drv mrk="pelmen.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      &Ccirc;indevena man&gcirc;a&jcirc;o, populara anka&ubreve; en pluraj
      landoj, inkluzive Rusion kaj Italion: bolkuiritaj malgrandaj kuketoj
      el pasto kun interna&jcirc;o el vianda, fi&scirc;a a&ubreve; legoma
      far&ccirc;o.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>pelmeni</trd>,
    <trd>meat dumplings</trd>,
    <trd>ravioli</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&kaf;&yod;&samekh;&fnun;</trd>,
    <trd>&resh;&bet;&yod;&vav;&lamed;&yod;</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>agnolotti</trd>,
    <trd>tortellini</trd>,
    <trd>cappeletti</trd>,
    <trd>ravioli</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>knoedel</trd>,
    <trd>ravioli</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_l;&c_mol;&c_m;&c_je;&c_n;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#39290;&#23376;</trd>,
    <trd><klr>tr. </klr>&#39171;&#23376;<klr> [ji&#462;ozi]</klr></trd>
  </trdgrp>
</drv>
</art>

