<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>fil</rad>/o
</kap>

<drv mrk="fil.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fil.0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Virseksa <ref tip="lst" cel="id.0o" lst="voko:parencoj">ido</ref>,
      rilate al la gepatroj:
      <ekz>
        hiera&ubreve; mi renkontis vian <tld/>on
        <fnt><bib>F</bib>, <lok>&FE; 20</lok></fnt>;
      </ekz>
      <ekz>
        Georgo instalis siajn pli a&gcirc;ajn <tld/>ojn &leftquot;en la
        Rusa lando&rightquot;<fnt><aut>Sergio Pokrovskij</aut>, <vrk>
        Moskvo jubileas</vrk>, <bib>Monato</bib>, <lok>jaro 1997a,
        numero 9a, p. 22a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_n;</trd>
  <trd lng="bg">&c_s;&c_i;&c_n;</trd>
  <trd lng="ca">fill</trd>
  <trdgrp lng="cs">
    <trd>f&yacute;la</trd>,
    <trd>syn</trd>
  </trdgrp>
  <trd lng="de">Sohn</trd>
  <trd lng="en">son</trd>
  <trd lng="es">hijo</trd>
  <trd lng="fr">fils</trd>
  <trd lng="fy">soan</trd>
  <trd lng="he">&bet;&fnun;</trd>
  <trd lng="hu">fi&uacute; <klr tip="amb">(vki fia)</klr></trd>
  <trd lng="id"><ind>anak</ind> laki-laki</trd>
  <trd lng="io">filiulo</trd>
  <trd lng="jbo">bersa</trd>
  <trd lng="nl">zoon</trd>
  <trd lng="pl">syn</trd>
  <trd lng="pt">filho</trd>
  <trd lng="ru">&c_s;&c_y;&c_n;</trd>
  <trd lng="sk">f&yacute;la</trd>
  <trd lng="tp">mije lili</trd>
  <trd lng="tr">o&gbreve;ul</trd>
  <trd lng="vo">son</trd>
  <trdgrp lng="zh">
    <trd>&#20799;&#23376;</trd>,
    <trd><klr>tr. </klr>&#20818;&#23376;<klr> [&eacute;rzi]</klr></trd>
  </trdgrp>
</drv>

<drv mrk="fil.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="fil.0igi.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Igi iun sia <tld/>o, <ref tip="vid" cel="adopt.0i">adopti</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_s;&c_y;&c_n;&c_a;&c_v;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_p;&c_r;&c_i;&c_je;&c_m;&c_a; &c_k;&c_a;&c_t;&c_o; &c_s;&c_i;&c_n;</trd>
  <trd lng="ca">afillar</trd>
  <trdgrp lng="cs">
    <trd>adoptovat syna</trd>,
    <trd>osvojit si syna</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>an Kindes statt annehmen</trd>,
    <trd>adoptieren</trd>
  </trdgrp>
  <trd lng="en">adopt</trd>
  <trd lng="es"><ind>adoptar</ind> un hijo</trd>
  <trd lng="fr">adopter<klr tip="ind"> (comme fils)</klr></trd>
  <trd lng="he">&lamed;<ind>&alef;&mem;&ftsadi;</ind></trd>
  <trd lng="hu">fi&aacute;v&aacute; fogad</trd>
  <trd lng="nl">als zoon <ind>adopteren</ind></trd>
  <trd lng="pl">usynawia&cacute;</trd>
  <trd lng="ru">&c_u;&c_s;&c_y;&c_n;&c_o;&c_v;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>adoptova&tcaron;</trd>,
    <trd>dc&eacute;ru</trd>,
    <trd>prija&tcaron; za syna</trd>
  </trdgrp>
  <trd lng="tr">evlatl&inodot;k edinmek</trd>
</drv>

<drv mrk="fil.0ino">
  <kap><ofc>*</ofc><tld/>ino</kap>
  <snc mrk="fil.0ino.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Inseksa <ref tip="lst" cel="id.0o" lst="voko:parencoj">ido</ref>, 
      rilate al la gepatroj:
      <ekz>
        tio &ccirc;i estis la unua fojo, ke &scirc;i nomis &scirc;in sia
        <tld/>ino <fnt><bib>F</bib>, <lok>&FE; 17, la feino</lok></fnt>;
      </ekz>
      <ekz>
        mia plej juna <tld/>ino naski&gcirc;is surda<fnt><aut>May
        Bijleveld</aut>, <vrk>Je defendo de liplegado</vrk>,
        <bib>Monato</bib>, <lok>jaro 2001a, numero 12a, p. 13a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_ch;&c_k;&c_a;</trd>
  <trd lng="bg">&c_d;&c_malmol;&c_shch;&c_je;&c_r;&c_ja;</trd>
  <trd lng="ca">filla</trd>
  <trd lng="cs">dcera</trd>
  <trd lng="de">Tochter</trd>
  <trd lng="en">daughter</trd>
  <trd lng="es">hija</trd>
  <trd lng="fr">fille<klr tip="ind"> (par rapport &agrave; ses
  parents)</klr></trd>
  <trd lng="fy">dochter</trd>
  <trd lng="he">&bet;&tav;</trd>
  <trd lng="hu">l&aacute;ny <klr tip="amb">(vki l&aacute;nya)</klr>
  </trd>
  <trd lng="id"><ind>anak</ind> perempuan</trd>
  <trd lng="io">filiino</trd>
  <trd lng="jbo">tixnu</trd>
  <trd lng="nl">dochter</trd>
  <trd lng="pl">c&oacute;rka</trd>
  <trd lng="pt">filha</trd>
  <trd lng="ru">&c_d;&c_o;&c_ch;&c_mol;</trd>
  <trd lng="sk">dc&eacute;ra</trd>
  <trd lng="tp">meli lili</trd>
  <trd lng="tr">k&inodot;z &ccedil;ocu&gbreve;u</trd>
  <trd lng="vo">daut</trd>
  <trdgrp lng="zh">
    <trd>&#22899;&#20799;</trd>,
    <trd><klr>tr. </klr>&#22899;&#20818;<klr> [n&#474;'&eacute;r]</klr>
    </trd>
  </trdgrp>
</drv>

<drv mrk="fil.0inigi">
  <kap><tld/>inigi</kap>
  <snc mrk="fil.0inigi.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Igi iun sia <tld/>ino, <ref tip="vid" cel="adopt.0i">adopti</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>an Kindes statt annehmen</trd>,
    <trd>adoptieren</trd>
  </trdgrp>
  <trd lng="en">adopt</trd>
  <trd lng="es"><ind>adoptar</ind> una hija</trd>
  <trd lng="nl">als dochter <ind>adopteren</ind></trd>
</drv>

<drv mrk="fil.0odelacxielo">
  <kap><tld/>o de la &ccirc;ielo</kap>
  <snc mrk="fil.0odelacxielo.HIS">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Honora nomo de reganto, precipe fama kiel titolo de la antikvaj
      &ccirc;inaj imperiestroj:
      <ekz>
        vi Oziriso Ramzes, eterne vivanta <tld/>o de la &ccirc;ielo,
        naskita de la diino Nut
        <fnt><bib>Far2</bib>, <lok>&Auml;&#8240;apitro 24a</lok></fnt>;
      </ekz>
      <ekz>
        la fe&ubreve;da imperiestro estas la <ctl><tld lit="F"/>o de la
        &Ccirc;ielo</ctl>, tute diferenca de la ordinaruloj
        <fnt><aut>&Ccirc;ina Radio Internacia</aut>,
        <vrk>Tian'anmen-placo de Pekino</vrk>,
        <lok>2004-04-20</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Sohn des Himmels<klr> (Herrschertitel)</klr></trd>
  <trd lng="fr">fils du ciel</trd>
  <trd lng="hu">az &eacute;g <ind>fia</ind></trd>
  <trd lng="ru">&c_s;&c_y;&c_n; &c_n;&c_je;&c_b;&c_a;</trd>
</drv>

<drv mrk="fil.bo0o">
  <kap>bo<tld/>o</kap>
  <snc mrk="fil.bo0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="lst" cel="edz.0o" lst="voko:parencoj">Edzo</ref> de la
      <tld/>(in)o:
      <ekz>
        Ned, kun la helpo de unu el siaj bo<tld/>oj, sekvis tiun
        instrukcion<fnt><aut>David Curtis</aut>, <vrk>Koratako</vrk>,
        <bib>Monato</bib>, <lok>jaro 2002a, numero 1a, p.
        28-34a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_ja;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_z;&c_je;&c_t;</trd>
  <trd lng="ca">gendre</trd>
  <trd lng="cs">ze&tcaron;</trd>
  <trd lng="de">Schwiegersohn</trd>
  <trd lng="en">son-in-law</trd>
  <trd lng="es">yerno</trd>
  <trdgrp lng="fr">
    <trd>beau-fils<klr> (gendre)</klr></trd>,
    <trd>gendre</trd>
  </trdgrp>
  <trd lng="fy">skoansoan</trd>
  <trd lng="hu">v&odblac;</trd>
  <trd lng="id">menantu</trd>
  <trd lng="io">bofiliulo</trd>
  <trd lng="nl">schoonzoon</trd>
  <trd lng="pl">zi&eogonek;&cacute;</trd>
  <trd lng="pt">genro</trd>
  <trd lng="ru">&c_z;&c_ja;&c_t;&c_mol;</trd>
  <trd lng="sk">za&tcaron;</trd>
  <trd lng="tr">damat</trd>
  <trd lng="vo">l&uuml;son</trd>
</drv>

<drv mrk="fil.bo0ino">
  <kap>bo<tld/>ino</kap>
  <snc mrk="fil.bo0ino.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="lst" cel="edz.0ino" lst="voko:parencoj">Edzino</ref> de
      la <tld/>(in)o:
      <ekz>
        iun diman&ccirc;on &scirc;i invitis al la tagman&gcirc;o
        amba&ubreve; estontajn bo<tld/>inojn<fnt><aut>Spomenka
        &Scirc;timec</aut>, <vrk>Morto en Melburna tramo</vrk>,
        <bib>Monato</bib>, <lok>jaro 1997a, numero
        8a,
        p. 24a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_ja;&c_v;&c_je;&c_s;&c_t;&c_k;&c_a;</trd>
  <trd lng="bg">&c_s;&c_n;&c_a;&c_h;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>nora</trd>,
    <trd>jove<klr tip="ind"> (nora)</klr></trd>
  </trdgrp>
  <trd lng="cs">snacha</trd>
  <trd lng="de">Schwiegertochter</trd>
  <trd lng="en">daughter-in-law</trd>
  <trd lng="es">nuera</trd>
  <trdgrp lng="fr">
    <trd>belle-fille<klr> (bru)</klr></trd>,
    <trd>bru</trd>
  </trdgrp>
  <trd lng="fy">skoandochter</trd>
  <trd lng="hu">meny</trd>
  <trd lng="id">menantu</trd>
  <trd lng="io">bofiliino</trd>
  <trd lng="nl">schoondochter</trd>
  <trd lng="pl">synowa</trd>
  <trd lng="pt">nora</trd>
  <trd lng="ru">&c_n;&c_je;&c_v;&c_je;&c_s;&c_t;&c_k;&c_a;</trd>
  <trd lng="sk">nevesta</trd>
  <trd lng="tr">gelin</trd>
  <trd lng="vo">l&uuml;daut</trd>
</drv>

<drv mrk="fil.duon0o">
  <kap>duon<tld/>o</kap>
  <snc mrk="fil.duon0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="lst" cel="fil.0o" lst="voko:parencoj"><tld
      lit="F"/>o</ref> el anta&ubreve;a edzi&gcirc;o, rilate al la nova
      edzo a&ubreve; edzino.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_y;&c_n;&c_a;&c_k;</trd>
  <trd lng="bg">&c_d;&c_o;&c_v;&c_je;&c_d;&c_je;&c_n; <ind>&c_s;&c_i;&c_n;</ind></trd>
  <trd lng="ca">fillastre</trd>
  <trd lng="cs">nevlastn&iacute; syn</trd>
  <trd lng="de">Stiefsohn</trd>
  <trd lng="en">stepson</trd>
  <trd lng="es">hijastro</trd>
  <trd lng="fr">beau-fils<klr tip="ind"> (fils du conjoint)</klr></trd>
  <trd lng="he">&bet;&fnun; &het;&vav;&resh;&gimel;</trd>
  <trd lng="hu">mostohafi&uacute;</trd>
  <trd lng="nl">stiefzoon</trd>
  <trd lng="pl">pasierb</trd>
  <trd lng="pt">enteado</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_y;&c_n;&c_o;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>nevlastn&yacute; syn</trd>,
    <trd>pastorok</trd>
  </trdgrp>
  <trd lng="tr">damat</trd>
</drv>

<drv mrk="fil.duon0ino">
  <kap>duon<tld/>ino</kap>
  <snc mrk="fil.duon0ino.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="lst" cel="fil.0ino" lst="voko:parencoj"><tld
      lit="F"/>ino</ref> el anta&ubreve;a edzi&gcirc;o, rilate al la nova
      edzo a&ubreve; edzino:
      <ekz>
        dume &scirc;i tenis sian duon<tld/>inon en severa servopreteco
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>dua libro, &ccirc;apitro
        33a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_ch;&c_a;&c_r;&c_k;&c_a;</trd>
  <trd lng="bg">&c_d;&c_o;&c_v;&c_je;&c_d;&c_je;&c_n;&c_a; <ind>&c_d;&c_malmol;&c_shch;&c_je;&c_r;&c_ja;</ind></trd>
  <trd lng="ca">fillastra</trd>
  <trd lng="cs">nevlastn&iacute; dcera</trd>
  <trd lng="de">Stieftochter</trd>
  <trd lng="en">stepdaughter</trd>
  <trd lng="es">hijastra</trd>
  <trd lng="fr">belle-fille<klr tip="ind"> (fille du conjoint)</klr>
  </trd>
  <trd lng="he">&bet;&tav; &het;&vav;&resh;&gimel;&tav;</trd>
  <trd lng="hu">mostohal&aacute;ny</trd>
  <trd lng="nl">stiefdochter</trd>
  <trd lng="pl">pasierbica</trd>
  <trd lng="pt">enteada</trd>
  <trd lng="ru">&c_p;&c_a;&c_d;&c_ch;&c_je;&c_r;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">nevlastn&aacute; dc&eacute;ra</trd>
  <trd lng="tr">gelin</trd>
</drv>



<drv mrk="fil.ge0oj">
  <kap><ofc>*</ofc>ge<tld/>oj</kap>
  <snc mrk="fil.ge0oj.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <tld lit="F"/>o(j) kaj <tld/>ino(j):
      <ekz>
        mia <tld/>o kaj <tld/>ino (ge<tld/>oj).
      </ekz>
    </dif>
    <trd lng="en">sons and daughters</trd>
    <trd lng="es">hijos y hijas</trd>
    <trd lng="fy">soannen en dochters</trd>
    <trd lng="io">gefilii</trd>
    <trd lng="nl">zonen en dochters</trd>
  </snc>
  <snc mrk="fil.ge0oj.RAR">
    <uzo tip="stl">RAR</uzo>
    <uzo tip="fak">GEN</uzo>
    <dif>
      <tld lit="F"/>o(j) a&ubreve;/kaj <tld/>ino(j):
      <ekz>
        &ccirc;u vi havas ge<tld/>ojn?
      </ekz>
    </dif>
    <trd lng="en">children</trd>
    <trd lng="fy">bern</trd>
    <trd lng="io">filii</trd>
    <trd lng="nl">kinderen</trd>
    <trd lng="tp">jan lili</trd>
    <trd lng="tr">&ccedil;ocuklar</trd>
    <trd lng="vo">cils</trd>
  </snc>
</drv>

<drv mrk="fil.ge0o">
  <kap>ge<tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="fil.ge0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="lst" cel="id.0o" lst="voko:parencoj">Ido</ref>, rilate
      al la gepatroj:
      <ekz>
        Jen ebla tasko, sendube pens-instiga, por gepatro kaj ge<tld/>o:
        kundiskuti la verkon.
        <fnt>
          <aut>Jorge Camacho</aut>.
          <lok>Monato</lok>
        </fnt>;
      </ekz>
    </dif>
    <rim>
      &Ccirc;i tiu uzo devias de la tradicia uzo, kaj ne &ccirc;iuj esperantistoj
      akceptas &gcirc;in. Vidu la <ref cel="ge.0.ununombro">duan sencon de
      ge-</ref>.
    </rim>
  </snc>
  <trd lng="en">child</trd>
  <trd lng="fy">bern</trd>
  <trd lng="io">filio</trd>
  <trd lng="jbo">panzi</trd>
  <trd lng="nl">kind</trd>
  <trd lng="tp">jan lili</trd>
  <trd lng="tr">&ccedil;ocuk</trd>
  <trd lng="vo">cil</trd>
</drv>
</art>
