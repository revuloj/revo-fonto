<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
 <ofc>*</ofc>
 <rad>fil</rad>/o
</kap>
<drv mrk="fil.0o">
 <kap><ofc>*</ofc><tld/>o</kap>
 <snc>
  <dif>
   Virseksa ido, rilate al la gepatroj.  
  </dif>
 </snc>
 <trd lng="cs">syn</trd>
 <trd lng="en">son</trd>
 <trd lng="es">hijo</trd>
 <trd lng="fr">fils</trd>
 <trd lng="de">Sohn</trd>
 <trd lng="nl">zoon</trd>
 <trd lng="pl">syn</trd>
 <trd lng="ru">&c_s;&c_y;&c_n;</trd>
 <trd lng="tr">o&gbreve;ul</trd>
</drv>
<drv mrk="fil.0ino">
 <kap><tld/>ino</kap>
 <snc>
  <dif>
   Inseksa ido, rilate al la gepatroj.  
  </dif>
 </snc>
 <trd lng="en">daughter</trd>
 <trd lng="ru">&c_d;&c_o;&c_ch;&c_mol;</trd>
</drv>
<drv mrk="fil.0igi">
 <kap><tld/>igi</kap>
 <snc>
  <dif>
   Igi iun sia <tld/>o, adopti. 
  </dif>
 </snc>
 <trd lng="nl">een zoon <ind>adopteren</ind></trd>
 <trd lng="ru">&c_u;&c_s;&c_y;&c_n;&c_o;&c_v;&c_i;&c_t;&c_mol;</trd>
</drv>
<drv mrk="fil.duon0o">
 <kap>duon<tld/>o</kap>
 <snc>
  <dif>
   <tld lit="F"/>o de anta&ubreve;a edzi&gcirc;o, rilate al la nova edzo a&ubreve; edzino.  
  </dif>
 </snc>
 <trd lng="nl">stiefzoon</trd>
 <trd lng="ru">&c_p;&c_a;&c_s;&c_y;&c_n;&c_o;&c_k;</trd>
</drv>
</art>
