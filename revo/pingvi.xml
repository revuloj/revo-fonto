<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pingvi.xml,v 1.19 2018/11/26 20:10:20 revo Exp $">
<kap>
  <rad>pingvin</rad>/o
</kap>
<drv mrk="pingvi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <uzo tip="stl">EVI</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="pingve.0o">Pingveno</ref>:
      <ekz>
        <nom>Drake</nom>-pasejo <klr>[&#8230;]</klr> &dash; karakterizaj bestoj estas cetacoj, <esc>delfinoj</esc>,
        albatrosoj a&ubreve; <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=243420"
              >Drake-pasejo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_n;&c_g;&c_v;&c_ib;&c_n;</trd>
  <trd lng="bg">&c_p;&c_i;&c_n;&c_g;&c_v;&c_i;&c_n;</trd>
  <trd lng="ca">ping&uuml;&iacute;</trd>
  <trdgrp lng="cs">
    <trd>pingv&iacute;n</trd>,
    <trd>tu&ccaron;&ncaron;&aacute;k</trd>
  </trdgrp>
  <trd lng="de">Pinguin</trd>
  <trd lng="en">penguin</trd>
  <trd lng="es">ping&uuml;ino</trd>
  <trd lng="fr">pingouin</trd>
  <trd lng="hu">pingvin</trd>
  <trd lng="nl">pingu&iuml;n</trd>
  <trd lng="pl">pingwin</trd>
  <trd lng="ru">&c_p;&c_i;&c_n;&c_g;&c_v;&c_i;&c_n;</trd>
  <trd lng="sk">tu&ccaron;niak</trd>
  <trd lng="sv">pingvin</trd>
</drv>
</art>
