<art mrk="$Id: api.xml,v 1.25 2022/04/25 15:10:59 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>api</rad>/o
</kap>
<drv mrk="api.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">Apium</trd>), la specio
      <ref tip="sub" cel="celeri.0o">celerio</ref> estas
      kulturata kiel legomo:
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> havas disajn, simplajn plumhavajn foliojn kaj umbelojn kun
        verdablankaj floroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=479443">Apio (genro)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/6/65/Apium_prostratum_kz4.jpg/512px-Apium_prostratum_kz4.jpg" prm="CC     BY-SA 4.0">specio de <tld/>o (Apium prostratum) en Hot Water Beach &ccirc;e Hahei, Coromandel
    (Nov-Zelando)
        <fnt>
          <aut>Krzysztof Ziarnek, Kenraiz, propra verko</aut>
          <lok><url ref="&WCW;/File:Apium_prostratum_kz4.jpg">&WCW;/File:Apium_prostratum_kz4.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <ref tip="hom" cel="api1.0o">Apio (&ccirc;efurbo)</ref>
  <trd lng="be">&c_s;&c_a;&c_l;&c_je;&c_r;&c_a;</trd>
  <trd lng="ca">api</trd>
  <trd lng="de">Sellerie</trd>
  <trdgrp lng="en">
    <trd>wild celery</trd>
  </trdgrp>
  <trd lng="es">apio</trd>
  <trd lng="fr">ache</trd>
  <trd lng="he">&#x5E1;&#x5DC;&#x5E8;&#x5D9;</trd>
  <trd lng="hu">zeller</trd>
  <trd lng="pl">selery</trd>
  <trd lng="ru">&c_s;&c_je;&c_l;&c_mol;&c_d;&c_je;&c_r;&c_je;&c_j;</trd>
  <trd lng="sv">floka</trd>
  <trd lng="uk">&#x434;&#x438;&#x43A;&#x430; &#x441;&#x435;&#x43B;&#x435;&#x440;&#x430;</trd>
</drv>
</art>
