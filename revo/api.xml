<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>api</rad>/o
</kap>
<drv mrk="api.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="api.0acoj">apiacoj</ref>
      (<trd lng="la">Apium</trd>), la specio 
      <ref tip="sub" cel="celeri.0o">celerio</ref> estas
      kulturata kiel legomo.      
    </dif>
  </snc>
  <trd lng="de">Sellerie</trd>
</drv>
<drv mrk="api.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de unujaraj dukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Apiaceae</trd>), kun umbelaj a&ubreve;
      duoble umbelaj infloreskoj.
    </dif>
  </snc>
  <trd lng="de">Doldenbl&uuml;ter</trd>
</drv>
</art>
<!--
$Log$
-->
</vortaro>