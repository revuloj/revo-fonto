<vortaro>
<art mrk="$Id: scientologi.xml,v 1.8 2024/04/17 07:23:30 revo Exp $">
<kap>
  <rad>Scientologi</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="scientologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="scientologi.0o.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      <ref tip="super" cel="sekt.0o.REL">Sekto</ref> fondita en Usono en 1953
      fare de <nom>L. R. Hubbard</nom>, pretendanta la plibonigon de
      la mensa stato de anoj:
      <ekz>
        &scirc;ajnas al mi ke <tld/>o estas granda stulta&jcirc;o,
        tamen apena&ubreve; pli ol aliaj religioj
        <fnt>
          <aut>Y. Bellefeuille</aut>,
          <vrk>Pri Scientologio</vrk>,
          <lok><url ref="&SCE;">
          soc.culture.esperanto</url>, 2001-08-31</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_je;&c_n;&c_t;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_S;&c_c;&c_i;&c_je;&c_n;&c_t;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="de">Scientology</trd>
  <trd lng="en">Scientology</trd>
  <trd lng="fr">Scientologie</trd>
  <trd lng="nl">Scientology</trd>
  <trd lng="pl">scjentologia</trd>
  <trdgrp lng="zh">
    <trd>&#x5C71;&#x8FBE;&#x57FA; <pr>Sh&amacron;nd&#xE1;j&#x12B;</pr></trd>,
    <trd>&#x5C71;&#36948;&#x57FA; <pr>Sh&amacron;nd&#xE1;j&#x12B;</pr></trd>,
    <trd>&#x5C71;&#x8FBE;&#x57FA;&#25945; <pr>sh&amacron;nd&#xE1;j&#x12B;ji&#xE0;o</pr></trd>,
    <trd>&#x5C71;&#36948;&#x57FA;&#25945; <pr>sh&amacron;nd&#xE1;j&#x12B;ji&#xE0;o</pr></trd>,
    <trd>&#x79D1;&#x5B66;&#x795E;&#25945; <pr>k&emacron;xu&#xE9;sh&#xE9;nji&#xE0;o</pr></trd>,
    <trd>&#x79D1;&#x5B78;&#x795E;&#25945; <pr>k&emacron;xu&#xE9;sh&#xE9;nji&#xE0;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="scientologi.0ano">
  <kap><tld/>ano</kap>
  <snc mrk="scientologi.0ano.REL">
    <dif>
      Ano de
      <ref tip="vid" cel="scientologi.0o.REL">scientologio</ref>:
      <ekz>
        <tld/>anoj diras, ke ili anta&ubreve;enigas religion
        surbaze de kompreno de la homa spirito
        <fnt>
          <aut>-</aut>,
          <vrk><url
          ref="http://eventeo.net/web/index.php/euxropo/socio/903-scientologio-e-tribunalo-riskas-malpermeson">
          Scientologio &ccirc;e tribunalo...</url></vrk>,
          <lok>eventeo, 2008-09-09</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_je;&c_n;&c_t;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="bg">&c_s;&c_c;&c_i;&c_je;&c_n;&c_t;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="de">scientologe</trd>
  <trd lng="en">scientologist</trd>
  <trd lng="fr">scientologue</trd>
  <trd lng="nl">scientoloog</trd>
  <trd lng="pl">scjentolog</trd>
</drv>
</art>

