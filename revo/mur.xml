<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc> <rad>mur</rad>/o</kap>
<drv mrk="mur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="mur.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Masona&jcirc;o pli alta ol dika, kiu konsistigas la flankojn de domo,
      &ccirc;irka&ubreve;as spacon a&ubreve; dividas ejon:
      <ekz>vivon privatan ka&scirc;u la <tld/>oj
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        alpremi iun al la <tld/>o<fnt>Z</fnt>
        <klr>(decigi iun respondi sen elturni&gcirc;o)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vand.0o">vando</ref>
    </refgrp>
  </snc>
  <snc mrk="mur.0o.metafora">
    <dif>
      Io simila al <tld/>o:
      <ekz>
        la krutaj <tld/>oj de la rokoj<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="bar.0o">baro</ref>,
      <ref cel="dig.0o">digo</ref>.
    </refgrp>
  </snc>
  <snc mrk="mur.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Netransirebla baro, neforigebla mallhelpa&jcirc;o, nevenkebla
      kontra&ubreve;staro:
      <ekz>
        nepenetrebla <tld/>o da rigardantoj;
      </ekz>
      <ekz>
        <tld/>o de malkompreno, de malfido.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_c;&c_ja;&c_n;&c_a;</trd>
  <trd lng="cs">ze&dcaron;</trd>
  <trd lng="de">Mauer</trd>
  <trd lng="en">wall</trd>
  <trd lng="fr">mur</trd>
  <trd lng="hu">fal</trd>
  <trd lng="nl">muur</trd>
  <trd lng="pl">&sacute;ciana</trd>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_n;&c_a;</trd>
  <trd lng="sv">v&auml;gg</trd>
</drv>

<drv mrk="mur.0ego">
  <kap><tld/>ego</kap>
  <uzo tip="fak">ARKI</uzo>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="rempar.0o.ARKI">Remparo</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_u;&c_r;</trd>,
    <trd>&c_c;&c_y;&c_t;&c_a;&c_d;&c_e;&c_l;&c_mol;&c_n;&c_y; &c_v;&c_a;&c_l;</trd>
  </trdgrp>
  <trd lng="hu">v&aacute;rfal</trd>
  <trd lng="sv">mur</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_je;&c_p;&c_o;&c_s;&c_t;&c_n;&c_a;&c_ja; &c_s;&c_t;&c_je;&c_n;&c_a;</trd>,
    <trd>&c_g;&c_o;&c_r;&c_o;&c_d;&c_s;&c_k;&c_a;&c_ja; &c_s;&c_t;&c_je;&c_n;&c_a;</trd>
  </trdgrp>
</drv>
</art>
