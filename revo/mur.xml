<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc> <rad>mur</rad>/o
</kap>
<drv mrk="mur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="mur.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Masona&jcirc;o pli alta ol dika, kiu konsistigas la flankojn de domo,
      &ccirc;irka&ubreve;as spacon a&ubreve; dividas ejon:
      <ekz>
        <ind>vivon privatan ka&scirc;u la <tld/>oj</ind>
        <trd lng="fr">pour vivre heureux vivons cach&eacute;s</trd>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>alpremi iun al la <tld/>o</ind>
        <trd lng="fr">mettre quelqu'un au pied du <ind>mur</ind></trd>
        <klr>(devigi iun respondi a&ubreve; decidi sen
        elturni&gcirc;o)</klr>
        <fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vand.0o">vando</ref>
    </refgrp>
  </snc>
  <snc mrk="mur.0o.metafora">
    <dif>
      Io simila al <tld/>o:
      <ekz>
        la krutaj <tld/>oj de la rokoj<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bar.0o">baro</ref>,
      <ref cel="dig.0o">digo</ref>
    </refgrp>
  </snc>
  <snc mrk="mur.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Netransirebla baro, neforigebla mallhelpa&jcirc;o, nevenkebla
      kontra&ubreve;staro:
      <ekz>
        nepenetrebla <tld/>o da rigardantoj;
      </ekz>
      <ekz>
        <tld/>o de malkompreno, de malfido.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_c;&c_ja;&c_n;&c_a;</trd>
  <trd lng="cs">ze&dcaron;</trd>
  <trd lng="de">Mauer</trd>
  <trd lng="en">wall</trd>
  <trd lng="fr">mur</trd>
  <trd lng="hu">fal</trd>
  <trd lng="id">dinding</trd>
  <trd lng="nl">muur</trd>
  <trd lng="pl">&sacute;ciana</trd>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_n;&c_a;</trd>
  <trd lng="sv">v&auml;gg</trd>
</drv>

<drv mrk="mur.0ego">
  <kap><tld/>ego</kap>
  <snc>
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Granda, fortika <ref tip="super" cel="mur.0o.ARKI">muro</ref> por
      &scirc;irmo kontra&ubreve; naturaj a&ubreve; homaj dan&gcirc;eroj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_u;&c_r;</trd>,
    <trd>&c_c;&c_y;&c_t;&c_a;&c_d;&c_e;&c_l;&c_mol;&c_n;&c_y;
    &c_v;&c_a;&c_l;</trd>
  </trdgrp>
  <trd lng="fr">muraille</trd>
  <trd lng="hu">v&aacute;rfal</trd>
  <trd lng="sv">mur</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_je;&c_p;&c_o;&c_s;&c_t;&c_n;&c_a;&c_ja;
    &c_s;&c_t;&c_je;&c_n;&c_a;</trd>,
    <trd>&c_g;&c_o;&c_r;&c_o;&c_d;&c_s;&c_k;&c_a;&c_ja;
    &c_s;&c_t;&c_je;&c_n;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="mur.cxirkaux0o">
  <kap>&ccirc;irka&ubreve;<tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;irka&ubreve;anta <tld/>o:
      <ekz>
        <klr>[la hom-amaso]</klr> pu&scirc;ante unu alian nevole
        piedpremis liajn herbejojn, rompis liajn arbojn, e&ccirc;
        difektis la &ccirc;irka&ubreve;muron <klr>[de lia domo]</klr>
        <fnt><bib>Far1</bib><lok>&ccirc;ap. 18</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">mur d'enceinte</trd>
</drv>

<drv mrk="mur.granda0o">
  <kap>Granda <tld lit="M"/>o</kap>
  <snc>
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Pli ol 6000 kilometrojn longa murego en la nordo de &Ccirc;inujo,
      poparte
      komencita por defendo de pli sudaj landoj, interligita en la 3a
      jarcento a. K. kaj plue konstruata en la sekvaj epokoj:
      <ekz>
        la Granda <tld lit="M"/>o <klr>[...]</klr> proksime de Pekino estas
        7-8 metrojn alta, 6-7 metrojn dika kaj 5 metrojn lar&gcirc;a
        &ccirc;e la supro
        <fnt><bib>EncCxi</bib>, <lok>&ccirc;apitro &leftquot;La Granda
        Muro&rightquot;, p.
        80a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">la <ind>Grande Muraille</ind><klr> (de Chine)</klr></trd>
</drv>
</art>
