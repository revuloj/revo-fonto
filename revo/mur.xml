<drv mrk="mur.0ego">
  <kap><tld/>ego</kap>
  <snc>
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Granda, fortika <ref tip="super" cel="mur.0o.ARKI">muro</ref> por
      &scirc;irmo kontra&ubreve; naturaj a&ubreve; homaj dan&gcirc;eroj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_u;&c_r;</trd>,
    <trd>&c_c;&c_y;&c_t;&c_a;&c_d;&c_e;&c_l;&c_mol;&c_n;&c_y;
    &c_v;&c_a;&c_l;</trd>
  </trdgrp>
  <trd lng="ca">muralla</trd>
  <trd lng="cs">hradba</trd>
  <trd lng="es">muralla</trd>
  <trd lng="fr">muraille</trd>
  <trd lng="hu">v&aacute;rfal</trd>
  <trdgrp lng="it">
    <trd>mura</trd>,
    <trd>muraglia</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ommuring</trd>,
    <trd>vestingmuur</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>mur obronny</trd>,
    <trd>wa&lstroke; ochronny</trd>
  </trdgrp>
  <trd lng="pt">muralha</trd>
  <trdgrp lng="ro">
    <trd>zid de ap&abreve;rare</trd>,
    <trd>baraj</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_je;&c_p;&c_o;&c_s;&c_t;&c_n;&c_a;&c_ja;
    &c_s;&c_t;&c_je;&c_n;&c_a;</trd>,
    <trd>&c_g;&c_o;&c_r;&c_o;&c_d;&c_s;&c_k;&c_a;&c_ja;
    &c_s;&c_t;&c_je;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">hradba</trd>
  <trd lng="sv">mur</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_u;&c_r;</trd>,
