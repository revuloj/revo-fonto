  <trd lng="bo">&#xF62;&#xFA9;&#xF72;&#xF42;&#xF0B;&#xF54;&#xF0B;</trd>
  <trd lng="bs">zid</trd>
  <trdgrp lng="ca">
    <trd>paret</trd>,
    <trd>mur</trd>
  </trdgrp>
  <trd lng="co">parete</trd>
  <trdgrp lng="cs">
    <trd>st&ecaron;na</trd>,
    <trd>ze&dcaron;</trd>
  </trdgrp>
  <trd lng="da">v&#xE6;g</trd>
  <trd lng="de">Mauer</trd>
  <trd lng="en">wall</trd>
  <trdgrp lng="es">
