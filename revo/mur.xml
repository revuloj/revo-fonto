<drv mrk="mur.0ego">
  <kap><tld/>ego <fnt><bib>JED</bib></fnt></kap>
  <snc>
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Granda, fortika <ref tip="super" cel="mur.0o.ARKI">muro</ref> por
      &scirc;irmo kontra&ubreve; naturaj a&ubreve; homaj dan&gcirc;eroj:
      <ekz>
        la <tld/>egoj de <nom>Jerusalem</nom>
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 3:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        vico super vico <klr>[&#x2026;]</klr> atakis la deklivojn, premis sin al la kastela
        <tld/>ego
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="rempar.0o.ARKI">remparo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_u;&c_r;</trd>,
    <trd>&c_c;&c_y;&c_t;&c_a;&c_d;&c_e;&c_l;&c_mol;&c_n;&c_y;
    &c_v;&c_a;&c_l;</trd>
  </trdgrp>
  <trd lng="ca">muralla</trd>
  <trd lng="cs">hradba</trd>
  <trd lng="de">Befestigungsmauer</trd>
  <trd lng="es">muralla</trd>
  <trd lng="fr">muraille</trd>
  <trd lng="hu">v&aacute;rfal</trd>
  <trdgrp lng="it">
    <trd>mura</trd>,
    <trd>muraglia</trd>
  </trdgrp>
  <trd lng="ja">&#x57CE;&#x58C1;  <pr>&#x3058;&#x3087;&#x3046;&#x3078;&#x304D;</pr></trd>
  <trdgrp lng="nl">
    <trd>ommuring</trd>,
