<drv mrk="mur.0eto">
  <kap><ofc>*</ofc><tld/>eto
  <fnt><bib>F</bib>, <lok>Universala Vortaro, -et'</lok></fnt></kap>
  <snc>
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Malalta <ref tip="super" cel="mur.0o.ARKI">muro</ref>, ofte uzata
      kiel limo de &gcirc;ardeno, kampo a&ubreve; pa&scirc;tejo.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_c;&c_je;&c_n;&c_k;&c_a;</trd>
  <trd lng="en"><klr>(low)</klr> stone wall</trd>
  <trd lng="fr">muret</trd>
  <trd lng="pl">murek</trd>
  <trd lng="ro">perete</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_ib;&#x43D;&#x43A;&c_a;</trd>,
    <trd>&#x43D;&c_je;&c_v;&c_i;&c_s;&#x43E;&#x43A;&c_a; &c_s;&c_t;&c_ib;&#x43D;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="mur.cxirkaux0o">
  <kap>&ccirc;irka&ubreve;<tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;irka&ubreve;anta <tld/>o:
      <ekz>
        <klr>[la hom-amaso]</klr> pu&scirc;ante unu alian nevole
        piedpremis liajn herbejojn, rompis liajn arbojn, e&ccirc;
        difektis la &ccirc;irka&ubreve;muron <klr>[de lia domo]</klr>
        <fnt><bib>Far1</bib><lok>&ccirc;ap. 18</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_g;&c_a;&c_r;&c_o;&c_d;&c_zh;&c_a;</trd>
  <trdgrp lng="es">
    <trd>cerca</trd>,
    <trd>cerco</trd>,
    <trd>cercado</trd>,
    <trd>tapia</trd>
  </trdgrp>
  <trd lng="fr">mur d'enceinte</trd>
  <trdgrp lng="it">
    <trd>mura</trd>,
    <trd><ind>cinta</ind> muraria</trd>,
    <trd><ind>muro</ind> di cinta</trd>
  </trdgrp>
  <trd lng="nl">ringmuur</trd>
  <trd lng="pl">ogrodzenie</trd>
  <trd lng="ro">gard</trd>
</drv>

<drv mrk="mur.granda0o">
  <kap>Granda <tld lit="M"/>o</kap>
  <snc>
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Pli ol 6000 kilometrojn longa murego en la nordo de &Ccirc;inujo,
      poparte
      komencita por defendo de pli sudaj landoj, interligita en la 3a
      jarcento a. K. kaj plue konstruata en la sekvaj epokoj:
      <ekz>
        la Granda <tld lit="M"/>o <klr>[...]</klr> proksime de Pekino
        estas
        7-8 metrojn alta, 6-7 metrojn dika kaj 5 metrojn lar&gcirc;a
        &ccirc;e la supro
        <fnt><bib>EncCxi</bib>, <lok>&ccirc;apitro &leftquot;La Granda
        Muro&rightquot;, p.
        80a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_V;&c_ja;&c_l;&c_ib;&c_k;&c_ib; &c_K;&c_ib;&c_t;&c_a;&c_j;&c_s;&c_k;&c_ib; <ind>&c_M;&c_u;&c_r;</ind></trd>
  <trd lng="ca">Gran Muralla</trd>
  <trd lng="es">la <ind>Gran muralla</ind> china</trd>
  <trd lng="de">Chinesische Mauer</trd>
  <trd lng="fr">la <ind>Grande Muraille</ind><klr> (de
  Chine)</klr></trd>
  <trd lng="it">grande <ind>muraglia</ind> cinese</trd>
  <trd lng="nl">Chinese <ind>muur</ind></trd>
  <trd lng="pl">Wielki <ind>Mur</ind> Chi&nacute;ski</trd>
  <trd lng="pt">a Grande <ind>Muralha</ind> da China</trd>
  <trd lng="ro">Marele Zid Chinezesc</trd>
</drv>
