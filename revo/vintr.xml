  <trd lng="sk">prezimova&tcaron;</trd>
  <trd lng="sv">&ouml;vervintra</trd>
  <trd lng="uk">&c_z;&c_i;&#x43C;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
