<drv mrk="vintr.tra0i">
  <kap>tra<tld/>i,
  <var><kap><tld/>i</kap></var></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="vintr.tra0i.KOMUNE">
    <dif>
      Lo&gcirc;i dum <tld/>o; pasigi la <tld/>on:
      <ekz>
        &ccirc;e vi eble mi restos, a&ubreve; e&ccirc; tra<tld/>os
        <fnt><bib>NT</bib>, <lok>&Ko1; 16:6</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;iaj bestoj de la tero super &gcirc;i <tld/>os
        <fnt><bib>MT</bib>, <lok>&Jes; 18:6</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_ib;&c_m;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_z;&c_ib;&c_m;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">hivernar</trd>
  <trd lng="cs">p&rcaron;ezimovat</trd>
  <trd lng="de">&uuml;berwintern</trd>
  <trd lng="fr">hiverner</trd>
  <trd lng="hu">&aacute;ttelel</trd>
  <trd lng="ja">&#x51AC;&#12434;&#x8D8A;&#12377; <pr>&#12405;&#12422;&#12434;&#12371;&#12377;</pr></trd>
