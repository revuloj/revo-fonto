  <trd lng="ca">hivern</trd>
  <trd lng="co">inguernu</trd>
  <trdgrp lng="cs">
    <trd>zima</trd>,
    <trd>zima <klr>(ro&ccaron;n&iacute; obdob&iacute;)</klr></trd>
  </trdgrp>
  <trd lng="da">vinter</trd>
