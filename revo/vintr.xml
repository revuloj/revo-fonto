<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vintr</rad>/o
</kap>
<drv mrk="vintr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Plej malvarma jarsezono, inter a&ubreve;tuno kaj printempo (en la norda
      hemisfero de proks. 21 dec. &gcirc;is 21 marto; en la suda de proks. 21
      jun. &gcirc;is 23 sep.):
      <ekz>
        kio ta&ubreve;gas por somero, ne ta&ubreve;gas por
        <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        marmoto <tld/>e dormas.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_ib;&c_m;&c_a;</trd>
  <trd lng="cs">zima</trd>
  <trd lng="de">Winter</trd>
  <trd lng="en">winter</trd>
  <trd lng="es">invierno</trd>
  <trd lng="fr">hiver</trd>
  <trd lng="hu">t&eacute;l</trd>
  <trd lng="nl">winter</trd>
  <trd lng="pl">zima</trd>
  <trd lng="ru">&c_z;&c_i;&c_m;&c_a;</trd>
  <trd lng="sv">vinter</trd>
</drv>

<drv mrk="vintr.0i">
  <kap><tld/>i<fnt>Z</fnt>, tra<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Lo&gcirc;i dum <tld/>o; pasigi la <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_ib;&c_m;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_z;i&c_m;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">&uuml;berwintern</trd>
  <trd lng="hu">&aacute;ttelel</trd>
  <trd lng="nl">overwinteren</trd>
  <trd lng="ru">&c_z;&c_i;&c_m;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">&ouml;vervintra</trd>
</drv>
</art>
