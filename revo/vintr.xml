<drv mrk="vintr.0a">
  <kap><tld/>a</kap>
  <snc mrk="vintr.0a.KAL">
    <dif>
      Okazanta en <tld/>o, per sia etoso simila al <tld/>o:
      <ekz>
        palaj <tld/>aj sunradioj
        <fnt><bib>InfanTorent2</bib>, <lok>unua parto, &ccirc;apitro
        2a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">hivernal</trd>
  <trd lng="cs">zimn&iacute;</trd>
  <trd lng="de">winterlich</trd>
  <trd lng="es">invernal</trd>
  <trd lng="fr">hivernal</trd>
  <trd lng="ja">&#x51AC;&#x306E; [<ind>&#x3075;&#x3086;&#x306E;</ind>]</trd>
  <trd lng="pl">zimowy</trd>
  <trd lng="sk">zimn&yacute;</trd>
</drv>

<drv mrk="vintr.tra0i">
  <kap>tra<tld/>i,
  <var><kap><tld/>i</kap></var></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="vintr.tra0i.KOMUNE">
    <dif>
      Lo&gcirc;i dum <tld/>o; pasigi la <tld/>on:
      <ekz>
        &ccirc;e vi eble mi restos, a&ubreve; e&ccirc; tra<tld/>os
        <fnt><bib>NT</bib>, <lok>&Ko1; 16:6</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;iaj bestoj de la tero super &gcirc;i <tld/>os
        <fnt><bib>MT</bib>, <lok>&Jes; 18:6</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_ib;&c_m;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_z;&c_ib;&c_m;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">hivernar</trd>
  <trd lng="cs">p&rcaron;ezimovat</trd>
  <trd lng="de">&uuml;berwintern</trd>
  <trd lng="fr">hiverner</trd>
  <trd lng="hu">&aacute;ttelel</trd>
  <trd lng="ja">&#x51AC;&#x3092;&#x8D8A;&#x3059; [<ind>&#x3075;&#x3086;&#x3092;&#x3053;&#x3059;</ind>]</trd>
  <trd lng="nl">overwinteren</trd>
  <trd lng="pl">przezimowa&cacute;</trd>
  <trd lng="ru">&c_z;&c_i;&c_m;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">prezimova&tcaron;</trd>
  <trd lng="sv">&ouml;vervintra</trd>
</drv>
