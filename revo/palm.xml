<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc><rad>palm</rad>/o</kap>
<drv mrk="palm.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Iu el <ref tip="malprt" cel="palm.0oj">palmoj</ref>.
    </dif>
  </snc>
  <bld lok="../bld/palm.jpg">
    <tld/>oj en Saharo
  </bld>
  <trd lng="de">Palme</trd>
  <trd lng="hu">p&aacute;lma</trd>
  <trd lng="nl">palm</trd>
  <trd lng="ru">&c_p;&c_a;&c_l;&c_mol;&c_m;&c_a;</trd>
  <trd lng="sv">palm</trd>
</drv>

<drv mrk="palm.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.unu0aj">unukotiledonaj</ref>
      plantoj (<trd lng="la">Arecaceae</trd>), plej ofte tropikaj arboj,
      kun senbran&ccirc;a trunko kaj densa krono da longaj
      &ccirc;iamverdaj folivicoj.
    </dif>
  </snc>
  <trd lng="de">Palmen</trd>
  <trd lng="hu">p&aacute;lm&aacute;k</trd>
  <trd lng="nl">palmen</trd>
  <trd lng="ru">&c_p;&c_a;&c_l;&c_mol;&c_m;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>

<drv mrk="palm.0ajxo0brancxo">
  <kap><tld/>a&jcirc;o, <tld/>bran&ccirc;o</kap>
  <snc>
    <dif>
      Bran&ccirc;o de <tld/>o.
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Signo de venko
    </dif>
    <refgrp tip="sin">
      <ref cel="lauxr.0o">la&ubreve;ro</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Palmwedel</trd>
  <trd lng="hu">p&aacute;lma&aacute;g</trd>
  <trd lng="nl">palmtak</trd>
  <trd lng="ru">&c_p;&c_a;&c_l;&c_mol;&c_m;&c_o;&c_v;&c_a;&c_ja; &c_v;&c_je;&c_t;&c_v;&c_mol;</trd>
</drv>

<drv mrk="palm.0ofesto0odimancxo">
  <kap><tld/>ofesto, <tld/>odiman&ccirc;o</kap>
  <uzo tip="fak">KRI</uzo>
  <snc>
    <dif>
      Festo dum kiu oni benas verdajn bran&ccirc;ojn por memorigi pri la
      eniro de Jesuo en Jerusalemon.
    </dif>
  </snc>
  <trd lng="de">Palmwedel</trd>
  <trd lng="hu">vir&aacute;gvas&aacute;rnap</trd>
  <trd lng="nl">Palmzondag</trd>
  <trd lng="ru">&c_v;&c_je;&c_r;&c_b;&c_n;&c_o;&c_je; &c_v;&c_o;&c_s;&c_k;&c_r;&c_je;&c_s;&c_je;&c_n;&c_mol;&c_je;</trd>
</drv>

<drv mrk="palm.0opieda">
  <kap><tld/>opieda</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Kvalifikas birdojn, kies piedfingroj estas kunigitaj per membrano
      (ansero, anaso, cigno, mevo); rempieda.
    </dif>
  </snc>
  <trd lng="hu">&uacute;sz&oacute;l&aacute;b&uacute;</trd>
  <trd lng="nl">met <ind>zwemvliezen</ind></trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_p;&c_o;&c_n;&c_ch;&c_a;&c_t;&c_o;&c_l;&c_a;&c_p;&c_y;&c_j;</trd>
</drv>
</art>
