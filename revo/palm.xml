<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>palm</rad>/o
</kap>
<drv mrk="palm.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Iu el <ref tip="malprt" cel="palm.oj">palmoj</ref>.
    </dif>
  </snc>
  <bld lok="../bld/palm.jpg"><tld/>oj en Saharo</bld>
  <trd lng="de">Palme</trd>
</drv>
<drv mrk="palm.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Ordo de unukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Arecaceae</trd>), plej ofte tropikaj arboj,
      kun senbran&ccirc;a trunko kaj densa krono da longaj
      &ccirc;iamverdaj folivicoj.
    </dif>
  </snc>
  <trd lng="de">Palmen</trd>
</drv>
<drv mrk="palm.0ajxo0brancxo">
  <kap><tld/>a&jcirc;o, <tld/>bran&ccirc;o</kap>
  <snc num="1">
    <dif>
      Bran&ccirc;o de <tld/>o. 
    </dif>
  </snc>
  <snc num="2">
    <uzo tip="klr">(f)</uzo>
    <dif>
      Signo de venko 
    </dif>
    <refgrp tip="sin">
      <ref cel="lauxr.0o">la&ubreve;ro</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Palmwedel</trd>
</drv>
<drv mrk="palm.0ofesto0odimancxo">
  <kap><tld/>ofesto, <tld/>odiman&ccirc;o</kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      Festo dum kiu oni benas verdajn bran&ccirc;ojn por memorigi pri la eniro
      de Jesuo en Jerusalemon.
    </dif>
  </snc>
  <trd lng="de">Palmsonntag</trd>
</drv>
<drv mrk="palm.0opieda">
  <kap><tld/>opieda</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Kvalifikas birdojn, kies piedfingroj estas kunigitaj per membrano
      (ansero, anaso, cigno, mevo); rempieda.
    </dif>
  </snc>
</drv>
</art>
