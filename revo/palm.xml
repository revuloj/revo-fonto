<vortaro>
<art mrk="$Id: palm.xml,v 1.40 2024/02/16 19:24:10 revo Exp $">
<kap>
  <ofc>*</ofc><rad>palm</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="palm.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="palm.0o.BOT">
    <dif>
      Arbo el <ref tip="malprt" cel="palm.0oj"><tld/>oj</ref>:
      <ekz>
        ili venis al <nom>Elim</nom>, tie estis dek du fontoj de akvo, kaj sepdek daktil<tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 15:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        iru sur la monton, kaj prenu <klr>[&#x2026;]</klr> bran&ccirc;ojn de <tld/>oj, kaj bran&ccirc;ojn
        de densfoliaj arboj, por fari la&ubreve;bojn
        <fnt>
          <bib>MT</bib>
          <lok>Ne&hcirc;emja 8:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        daktil<tld/>o etendis sian &scirc;irman foliaron tre alte, kvaza&ubreve; &gcirc;i volus ludi la
        rolon de sunombrelo
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi estas kvaza&ubreve; alta <tld/>o, kaj la lotuso envias vian
        gracion
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 9a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="th">
      <trd>&#xE1B;&#xE32;&#xE25;&#xE4C;&#xE21;</trd>,
      <trd>&#xE15;&#xE49;&#xE19;&#xE1B;&#xE32;&#xE25;&#xE4C;&#xE21;</trd>
    </trdgrp>
  </snc>
  <bld lok="../bld/palm.jpg">
    <tld/>oj en Saharo
  </bld>
  <trd lng="be">&c_p;&c_a;&c_l;&c_mol;&c_m;&c_a;</trd>
  <trd lng="bg">&c_p;&c_a;&c_l;&c_m;&c_a;</trd>
  <trd lng="ca">palmera</trd>
  <trd lng="de">Palme</trd>
  <trd lng="en">palm</trd>
  <trdgrp lng="es">
    <trd>palma</trd>,
    <trd>palmera</trd>
  </trdgrp>
  <trd lng="fr">palmier</trd>
  <trd lng="hu">p&aacute;lma</trd>
  <trd lng="id">palem</trd>
  <trdgrp lng="ja">
    <trd>&#x30E4;&#x30B7;</trd>,
    <trd>&#x30B7;&#x30E5;&#x30ED;</trd>
  </trdgrp>
  <trd lng="nl">palm</trd>
  <trd lng="pl">palma</trd>
  <trd lng="pt">palmeira</trd>
  <trd lng="ro">palmier</trd>
  <trd lng="ru">&c_p;&c_a;&c_l;&c_mol;&c_m;&c_a;</trd>
  <trd lng="sv">palm</trd>
  <trd lng="uk">&#x43F;&c_a;&#x43B;&#x44C;&#x43C;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x68D5;&#x6988; <pr>z&#x14D;ngl&#xFA;:</pr></trd>,
    <trd>&#x68D5;&#x6ADA; <pr>z&#x14D;ngl&#xFA;:</pr></trd>,
    <trd>&#x6988; <pr>l&#xFA;:</pr></trd>,
    <trd>&#x6ADA; <pr>l&#xFA;:</pr></trd>,
    <trd>&#x68D5;&#x6811; <pr>z&#x14D;ngsh&#xF9;</pr></trd>,
    <trd>&#x68D5;&#x6A39; <pr>z&#x14D;ngsh&#xF9;</pr></trd>,
    <trd>&#x6930;&#x6811; <pr>y&#xE9;sh&#xF9;</pr></trd>,
    <trd>&#x6930;&#x6A39; <pr>y&#xE9;sh&#xF9;</pr></trd>,
    <trd>&#x6988; <pr>l&#x1D8;</pr></trd>,
    <trd>&#x6ADA; <pr>l&#x1D8;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="palm.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="palm.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Plej ofte tropikaj arboj kun senbran&ccirc;a trunko kaj densa krono da longaj &ccirc;iamverdaj folivicoj,
      <ref tip="lst" lst="voko:botanikaj_familioj" cel="famili.0o.BIO">familio</ref>
       de <ref tip="malprt" cel="kotile.unu0aj">unukotiledonaj</ref> vegeta&jcirc;oj (<trd lng="la">Arecaceae</trd>) kun botanika nomo <ref tip="sin" cel="arekac.0oj">arekacoj</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_mol;&c_m;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_p;&c_a;&c_l;&c_m;&c_o;&c_v;&c_i;</trd>
  <trdgrp lng="ca">
    <trd>palmes</trd>,
    <trd>arec&agrave;cies</trd>
  </trdgrp>
  <trd lng="de">Palmen</trd>
  <trd lng="en">palms</trd>
  <trd lng="fr">palmiers</trd>
  <trd lng="hu">p&aacute;lm&aacute;k</trd>
  <trd lng="nl">palmen</trd>
  <trd lng="pl">palmy</trd>
  <trd lng="ro">palmieri</trd>
  <trd lng="ru">&c_p;&c_a;&c_l;&c_mol;&c_m;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&#x43F;&c_a;&#x43B;&#x44C;&#x43C;&#x43E;&c_v;&c_ib;</trd>
</drv>

<drv mrk="palm.0obrancxo">
  <kap><tld/>obran&ccirc;o, <var><kap><tld/>a&jcirc;o</kap></var></kap>
  <snc mrk="palm.0obrancxo.KOMUNE">
    <dif>
      Bran&ccirc;o de <tld/>o:
      <ekz>
        rande de la domaro, &scirc;irmita de la <tld/>obran&ccirc;oj,
        estis la kabano, kien la virinoj retiri&gcirc;as dum
        menstruado
        <fnt>
          <aut>W. Schad</aut>
          <vrk>Orinoko-delto...</vrk>
          <bib>Monato</bib>
          <lok>2000:11, p. 20a</lok>
        </fnt>;
      </ekz>
      <ekz>
        tri dometoj, kies teraj muroj kaj <tld/>a&jcirc;aj tegmentoj alkro&ccirc;i&gcirc;as al la digo
        <fnt>
          <bib>MD05</bib>
          <vrk>Piratoj en la magloarbaro forkaptas kaj batas la fi&scirc;kaptistojn</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="palm.0obrancxo.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Signo de venko.
    </dif>
    <refgrp tip="vid">
      <ref cel="lauxr.0oj.gloro">la&ubreve;ro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_mol;&c_m;&c_a;&c_v;&c_a;&c_ja; &c_g;&c_a;&c_l;&c_ib;&c_n;&c_a;</trd>
  <trd lng="ca">palma <klr>(fulla de palmera)</klr></trd>
  <trd lng="de">Palmwedel</trd>
  <trd lng="en">palm frond</trd>
  <trd lng="fr">palme</trd>
  <trd lng="hu">p&aacute;lma&aacute;g</trd>
  <trd lng="ja">&#x30E4;&#x30B7;&#x306E;&#x8449;  <pr>&#x3084;&#x3057;&#x306E;&#x306F;</pr></trd>
  <trd lng="nl">palmtak</trd>
  <trdgrp lng="pl">
    <trd>li&sacute;&cacute;  <ind>palmowy</ind></trd>,
    <trd>ga&lstroke;&aogonek;zka  <ind>palmowa</ind></trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>frunza de  <ind>palmier</ind></trd>,
    <trd>ramur&abreve; de  <ind>palmier</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_l;&c_mol;&c_m;&c_o;&c_v;&c_a;&c_ja;
  &c_v;&c_je;&c_t;&c_v;&c_mol;</trd>
  <trd lng="zh">&#x8549;&#x6247; <pr>ji&#x1CE;osh&#xE0;n</pr></trd>
</drv>

<drv mrk="palm.0ofesto">
  <kap><tld lit="P"/>ofesto, <var><kap><tld lit="P"/>odiman&ccirc;o</kap></var></kap>
  <snc mrk="palm.0ofesto.KRI">
    <uzo tip="fak">KAL</uzo>
    <uzo tip="fak">KRI</uzo>
    <dif>
      <ref tip="lst" cel="fest.0o" lst="voko:festoj">Festo</ref>
      memoriganta la eniron de Jesuo en Jerusalemon sub
      honore svingataj bran&ccirc;oj de <tld/>oj:
      <ekz>
        en la Moskva Regno la patriarko je la <tld lit="P"/>odiman&ccirc;o venadis rajdante
        sur azeno kiun piede kondukis la caro
        <fnt>
          <bib>LOdE</bib>
          <vrk><url ref="&Tekstaro;?nomo=ondo-de-esperanto">La tri epokfaraj
          eltrovoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_je;&c_r;&c_b;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="bg">&c_c;&c_v;&c_je;&c_t;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="ca">diumenge de Rams</trd>
  <trd lng="de">Palmsonntag</trd>
  <trd lng="en">Palm Sunday</trd>
  <trd lng="es">Domingo de Ramos</trd>
  <trd lng="fr">dimanche des rameaux</trd>
  <trd lng="hu">vir&aacute;gvas&aacute;rnap</trd>
  <trd lng="id"><ind>Minggu</ind> Palma</trd>
  <trd lng="ja">&#x68D5;&#x6ADA;&#x306E;&#x8056;&#x65E5; 
  <pr>&#x3057;&#x3085;&#x308D;&#x306E;&#x3072;&#x3058;&#x308A;&#x306B;&#x3061;</pr></trd>
  <trd lng="nl">Palmzondag</trd>
  <trd lng="pl">Niedziela  <ind>Palmowa</ind></trd>
  <trd lng="ro">Duminica Floriilor</trd>
  <trd lng="ru">&c_v;&c_je;&c_r;&c_b;&c_n;&c_o;&c_je; &c_v;&c_o;&c_s;&c_k;&c_r;&c_je;&c_s;&c_je;&c_n;&c_mol;&c_je;</trd>
  <trdgrp lng="th">
    <trd>&#xE27;&#xE31;&#xE19;&#xE2D;&#xE32;&#xE17;&#xE34;&#xE15;&#xE22;&#xE4C;&#xE43;&#xE1A;&#xE25;&#xE32;&#xE19;</trd>,
    <trd>&#xE27;&#xE31;&#xE19;&#xE2D;&#xE32;&#xE17;&#xE34;&#xE15;&#xE22;&#xE4C;&#xE17;&#xE32;&#xE07;&#xE15;&#xE32;&#xE25;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x68D5;&#x6811;&#x4E3B;&#x65E5; <pr>z&#x14D;ngsh&#xF9;zh&#x1D4;r&#xEC;</pr></trd>,
    <trd>&#x68D5;&#x6A39;&#x4E3B;&#x65E5; <pr>z&#x14D;ngsh&#xF9;zh&#x1D4;r&#xEC;</pr></trd>,
    <trd>&#x68D5;&#x679D;&#x4E3B;&#x65E5; <pr>z&#x14D;ngzh&#x12B;zh&#x1D4;r&#xEC;</pr></trd>,
    <trd>&#x68D5;&#x679D;&#x5168;&#x65E5; <pr>z&#x14D;ngzh&#x12B;qu&#xE1;nr&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="palm.0opieda">
  <kap><tld/>opieda</kap>
  <snc mrk="palm.0opieda.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Havanta piedfingrojn kunigitajn per membrano, kiel iuj
      na&gcirc;obestoj (ansero, anaso...):
      <ekz>
        junuloj <klr>[&#x2026;]</klr> kuni&gcirc;as en parkoj, <klr>[&#x2026;]</klr> ili portas bazopilkajn
        &ccirc;apojn kaj blankajn <tld/>opied-similajn tol&scirc;uojn: kiel sencerbaj anasoj ili
        aspektas
        <fnt>
          <aut>Garbhan MacAoidh</aut>
            <vrk><url ref="&Monato;/2000/007076.php">Ho, tempoj! Ho, moroj!</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 19</lok>
          </fnt>;
      </ekz>
      <ekz>
        ili ne tute estis korvoj, nek talpoj, <klr>[&#x2026;]</klr>, ili lame kaj neegale
        iradis, duone per siaj <tld/>opiedoj kaj duone per siaj membranaj flugiloj
        <fnt>
          <aut>Sonja Lang</aut>
          <vrk>La Soleno</vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="rem.0pieda">rempieda</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_p;&c_ch;&c_a;&c_s;&c_t;&c_a;&c_n;&c_o;&c_g;&c_ib;</trd>,
    <trd>&c_v;&c_je;&c_s;&c_l;&c_a;&c_n;&c_o;&c_g;&c_ib;</trd>
  </trdgrp>
  <trd lng="ca">palm&iacute;pede</trd>
  <trd lng="en">palmiped</trd>
  <trd lng="es">palm&iacute;pedo</trd>
  <trd lng="fr">palmip&egrave;de<klr> (adj.)</klr></trd>
  <trd lng="hu">&uacute;sz&oacute;l&aacute;b&uacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x6C34;&#x304B;&#x304D;&#x306E;&#x3042;&#x308B; 
    <pr>&#x307F;&#x305A;&#x304B;&#x304D;&#x306E;&#x3042;&#x308B;</pr></trd>,
    <trd>&#x8E7C;&#x8DB3;&#x306E; <pr>&#x307C;&#x304F;&#x305D;&#x304F;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">met <ind>zwemvliezen</ind></trd>
  <trd lng="pl">z palcami po&lstroke;&aogonek;czonymi b&lstroke;on&aogonek; p&lstroke;awn&aogonek;</trd>
  <trd lng="ro">cu degetele conectate printr-o membran&abreve;</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_p;&c_o;&c_n;&c_ch;&c_a;&c_t;&c_o;&c_l;&c_a;&c_p;&c_y;&c_j;</trd>
  <trd lng="uk">&#x43F;&c_je;&c_r;&c_je;&c_t;&c_i;&#x43D;&c_ch;&c_a;&c_s;&c_t;&#x43E;&#x43B;&c_a;&#x43F;&c_i;&c_j;</trd>
</drv>
