<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <rad>adolt</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="adolt.0o">
   <kap><tld/>o</kap>
   <snc>
     <ref tip="dif" cel="kresk.plen0ulo">plenkreskulo</ref>
     <ekz>
       <tld/>oj ne estis en la &ccirc;ambro
       <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 21a, p. 208a</lok></fnt>.
     </ekz>
   </snc>
   <trdgrp lng="be">
     <trd>&c_d;&c_a;&c_r;&c_o;&c_s;&c_l;&c_y; <klr>(&c_n;&c_a;&c_z;.)</klr></trd>,
     <trd>&c_s;&c_t;&c_a;&c_l;&c_y; <klr>(&c_n;&c_a;&c_z;.)</klr></trd>
   </trdgrp>
   <trd lng="fr">adulte<klr> (subst.)</klr></trd>
   <trd lng="ru">&c_v;&c_z;&c_r;&c_o;&c_s;&c_l;&c_y;&c_j;<klr> (&c_s;&c_u;&c_shch;.)</klr></trd>
</drv>

<drv mrk="adolt.0a">
   <kap><tld/>a</kap>
   <uzo tip="klr">(pri vivulo)</uzo>
   <snc>
     <dif>
       <ref tip="super" cel="matur.0a">Matura</ref>, atinginta sian plenan
       a&gcirc;on:
       <ekz>
         li <klr>...</klr> postlasis &scirc;in, forirante al <tld/>a vivo en la
         urbo
         <fnt><aut>Gerrit Berveling</aut>: <vrk>Iom tro predikema romano
         (Lev Tolstoj, Resurekto)</vrk>, <bib>Monato</bib>, <lok>jaro
         2001a, numero 5a, p. 30a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_d;&c_a;&c_r;&c_o;&c_s;&c_l;&c_y;</trd>,
     <trd>&c_s;&c_t;&c_a;&c_l;&c_y;</trd>
   </trdgrp>
   <trd lng="fr">adulte<klr> (adj.)</klr></trd>
   <trdgrp lng="ru">
     <trd>&c_v;&c_z;&c_r;&c_o;&c_s;&c_l;&c_y;&c_j;</trd>,
     <trd>&c_z;&c_r;&c_je;&c_l;&c_y;&c_j;</trd>
   </trdgrp>
</drv>

<drv mrk="adolt.0eco">
   <kap><tld/>eco</kap>
   <snc>
     <dif>
       Homa matureco, plenkreskula situacio a&ubreve; ecaro.
     </dif>
   </snc>
   <trd lng="be">&c_s;&c_t;&c_a;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
   <trd lng="ru">&c_z;&c_r;&c_je;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="adolt.0igxi">
   <kap><tld/>i&gcirc;i</kap>
   <gra><vspec>ntr</vspec></gra>
   <snc>
     <dif>
       Ekatingi la a&gcirc;on, la kapablojn de plenkreskulo.
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_s;&c_t;&c_a;&c_l;&c_je;&c_c;&c_mol;</trd>,
     <trd>&c_p;&c_a;&c_s;&c_t;&c_a;&c_l;&c_je;&c_c;&c_mol;</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>devenir adulte</trd>,
     <trd>grandir<klr> (devenir adulte)</klr></trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_v;&c_z;&c_r;&c_o;&c_s;&c_l;&c_je;&c_t;&c_mol;</trd>,
     <trd>&c_p;&c_o;&c_v;&c_z;&c_r;&c_o;&c_s;&c_l;&c_je;&c_t;&c_mol;</trd>
   </trdgrp>
</drv>

<drv mrk="adolt.0igxo">
   <kap><tld/>i&gcirc;o</kap>
   <snc>
     <dif>
       Maturi&gcirc;o de homo atinganta plenkreskon.
     </dif>
   </snc>
   <trd lng="be">&c_s;&c_t;&c_a;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
   <trd lng="fr">croissance<klr> (devenir adulte)</klr></trd>
   <trd lng="ru">&c_v;&c_z;&c_r;&c_o;&c_s;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>
</art>

