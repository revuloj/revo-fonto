<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: adolt.xml,v 1.18 2023/10/27 13:42:09 revo Exp $">
<kap>
  <rad>adolt</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="adolt.0o">
  <kap><tld/>o</kap>
  <snc mrk="adolt.0o.plenkreskulo">
    <dif>
      <ref tip="dif" cel="kresk.plen0ulo">Plenkreskulo</ref>:
      <ekz>
        <tld/>oj ne estis en la &ccirc;ambro
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 21a, p. 208a</lok></fnt>;
      </ekz>
      <ekz>
        miaj lernejanoj estas nun <tld/>oj, geedzoj kaj gepatroj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2015/011597.php">&Scirc;ton&jcirc;eto sen cenzuro</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        plimulto de la malsanuloj estas infanoj kaj adoleskantoj, kvankam <tld/>oj mem
        povas malsani&gcirc;i, se ili ekzemple ne estis infektitaj dum la propra infana&gcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2012/010767.php">Elimino atingeblas</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x6210;&#x5E74;&#x4EBA; <pr>ch&#xE9;ngni&#xE1;nr&#xE9;n</pr></trd>,
    <trd>&#x6210;&#x5E74;&#x8005; <pr>ch&#xE9;ngni&#xE1;nzh&#x11B;</pr></trd>
  </trdgrp>
</snc>
  <rim>
    Ne konfuzu kun <ref tip="hom" cel="adult.0o">adulto</ref>
    (rompo de geedza fideleco).
  </rim>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_r;&c_o;&c_s;&c_l;&c_y; 
<klr>(&c_n;&c_a;&c_z;.)</klr></trd>,
    <trd>&c_s;&c_t;&c_a;&c_l;&c_y; <klr>(&c_n;&c_a;&c_z;.)</klr></trd>
  </trdgrp>
  <trd lng="ca">adult <klr>(subst.)</klr></trd>
  <trd lng="cs">dosp&ecaron;l&yacute; jedinec</trd>
  <trdgrp lng="de">
    <trd>Erwachsener</trd>,
    <trd>Vollj&auml;hriger</trd>
  </trdgrp>
  <trd lng="es">adulto</trd>
  <trd lng="fr">adulte<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="he">&#x5DE;&#x5B0;&#x5D1;&#x5D5;&#x5BC;&#x5D2;&#x5B8;&#x5E8;</trd>
  <trd lng="it">adulto<klr> (sost.)</klr></trd>
  <trd lng="nl">volwassene</trd>
  <trd lng="pl">doros&lstroke;y</trd>
  <trd lng="pt">adulto</trd>
  <trd lng="ro">adult</trd>
  <trd lng="ru">&c_v;&c_z;&c_r;&c_o;&c_s;&c_l;&c_y;&c_j;<klr> 
(&c_s;&c_u;&c_shch;.)</klr></trd>
  <trd lng="sk">dospel&yacute; jedinec</trd>
</drv>

<drv mrk="adolt.0a">
  <kap><tld/>a, <var><kap>adolta&gcirc;a</kap></var></kap>
  <snc mrk="adolt.0a.plenkreska">
    <dif>
      <ref tip="dif" cel="kresk.plen0a">Plenkreska</ref>:
      <ekz>
        li <klr>[&#x2026;]</klr> postlasis &scirc;in, forirante al <tld/>a vivo 
        en la urbo
        <fnt><aut>Gerrit Berveling</aut>: <vrk>Iom tro predikema romano
        (Lev Tolstoj, Resurekto)</vrk>, <bib>Monato</bib>, <lok>jaro
        2001a, numero 5a, p. 30a</lok></fnt>;
      </ekz>
      <ekz>
        la individuo estas libera sin ligi a&ubreve; ne ligi al kiu ajn alia <tld/>a homo, &ccirc;u
        gee, &ccirc;u geje
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="&Monato;/2006/008952.php">Toleremo kaj liberecoj en Nederlando</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a&gcirc;e li realigas tiun revon
        <fnt>
          <bib>Monato</bib>
          <aut>Antonio Val&eacute;n</aut>
          <vrk><url ref="&Monato;/2005/008499.php">&Ccirc;u Ertl provizore stilekzercas?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_r;&c_o;&c_s;&c_l;&c_y;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_l;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">adult <klr>(adj.)</klr></trd>
  <trd lng="cs">dosp&ecaron;l&yacute;</trd>
  <trdgrp lng="de">
    <trd>erwachsen</trd>,
    <trd>vollj&auml;hrig</trd>
  </trdgrp>
  <trd lng="es">adulto</trd>
  <trd lng="fr">adulte<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="he">&#x5DE;&#x5B0;&#x5D1;&#x5D5;&#x5BC;&#x5D2;&#x5B8;&#x5E8;</trd>
  <trd lng="it">adulto<klr> (agg.)</klr></trd>
  <trdgrp lng="nl">
    <trd>volwassen</trd>,
    <trd>volgroeid</trd>
  </trdgrp>
  <trd lng="pl">doros&lstroke;y</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_z;&c_r;&c_o;&c_s;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_z;&c_r;&c_je;&c_l;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">dospel&yacute;</trd>
  <trd lng="uk">&#x434;&#x43E;&#x440;&#x43E;&#x441;&#x43B;&#x438;&#x439;</trd>
</drv>

<drv mrk="adolt.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="adolt.0eco.stato">
    <dif>
      Stato de <tld/>o:
      <ekz>
        preska&ubreve; &ccirc;iu esperantisto, jam en sia infaneco a&ubreve; juna <tld/>eco, familiari&gcirc;is
        kun <klr>[<nom>Jules Verne</nom>]</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2009/009860.php">Voja&gcirc;romano klasika</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        sursojle al <tld/>eco, ili eniras strangan, pli lar&gcirc;an mondon
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2011/010515.php">Bonvenon al nova studjaro</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="ca">adultesa</trd>
  <trd lng="cs">dosp&ecaron;lost</trd>
  <trdgrp lng="de">
    <trd>Vollj&auml;hrigkeit</trd>,
    <trd>Erwachsensein</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&acirc;ge <ind>adulte</ind></trd>,
    <trd>maturit&eacute;<klr tip="ind"> (&acirc;ge adulte)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>et&agrave; <ind>adulta</ind></trd>,
    <trd>maturit&agrave;<klr tip="ind"> (et&agrave; adulta)</klr></trd>
  </trdgrp>
  <trd lng="nl">volwassenheid</trd>
  <trd lng="pl">doros&lstroke;o&sacute;&cacute;</trd>
  <trd lng="ru">&c_z;&c_r;&c_je;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">dospelos&tcaron;</trd>
</drv>

<drv mrk="adolt.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="adolt.0igxi.ekatingi">
    <dif>
      Ekatingi la a&gcirc;on, la staton de <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_l;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_t;&c_a;&c_l;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">fer-se adult</trd>
  <trdgrp lng="de">
    <trd><ind>erwachsen</ind> werden</trd>,
    <trd>heranwachsen</trd>,
    <trd><ind>vollj&auml;hrig</ind> werden</trd>
  </trdgrp>
  <trd lng="es">ser adulto</trd>
  <trdgrp lng="fr">
    <trd>devenir <ind>adulte</ind></trd>,
    <trd>grandir<klr tip="ind"> (devenir adulte)</klr></trd>,
    <trd>m&ucirc;rir<klr tip="ind"> (devenir adulte)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>diventare <ind>adulto</ind></trd>,
    <trd>maturare<klr tip="ind"> (diventare adulto)</klr></trd>
  </trdgrp>
  <trd lng="pl">dorosn&aogonek;&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_z;&c_r;&c_o;&c_s;&c_l;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_v;&c_z;&c_r;&c_o;&c_s;&c_l;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="adolt.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc mrk="adolt.0igxo.transiro">
    <dif>
      Transiro al <tld/>eco:
      <ekz>
        ili ricevas seksan edukon kaj alproprigas utilajn kapablojn cele al sia <tld/>i&gcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Adj&eacute; Adj&eacute;vi</aut>
          <vrk><url ref="&Monato;/2012/010618.php">Kutamaku, la lando de Betamariba</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="ca">
    <trd>maduraci&oacute;</trd>,
    <trd>pas a l'adultesa</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>croissance<klr tip="ind"> (devenir adulte)</klr></trd>,
    <trd>maturation<klr tip="ind"> (devenir adulte)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>diventare <ind>adulto</ind></trd>,
    <trd>maturazione<klr tip="ind"> (diventare adulto)</klr></trd>
  </trdgrp>
  <trd lng="nl">het <ind>volwassen</ind> worden</trd>
  <trd lng="pl">dorastanie</trd>
  <trd lng="ru">&c_v;&c_z;&c_r;&c_o;&c_s;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>
</art>
