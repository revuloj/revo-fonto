<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>adolt</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="adolt.0o">
  <kap><tld/>o</kap>
  <snc>
    <ref tip="dif" cel="kresk.plen0ulo">plenkreskulo</ref>
    <ekz>
      <tld/>oj ne estis en la &ccirc;ambro
      <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 21a, p. 208a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_r;&c_o;&c_s;&c_l;&c_y; 
<klr>(&c_n;&c_a;&c_z;.)</klr></trd>,
    <trd>&c_s;&c_t;&c_a;&c_l;&c_y; <klr>(&c_n;&c_a;&c_z;.)</klr></trd>
  </trdgrp>
  <trd lng="de">Erwachsener</trd>
  <trd lng="es">adulto</trd>
  <trd lng="fr">adulte<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="nl">volwassene</trd>
  <trd lng="pl">doros&lstroke;y</trd>
  <trd lng="pt">adulto</trd>
  <trd lng="ro">adult</trd>
  <trd lng="ru">&c_v;&c_z;&c_r;&c_o;&c_s;&c_l;&c_y;&c_j;<klr> 
(&c_s;&c_u;&c_shch;.)</klr></trd>
</drv>

<drv mrk="adolt.0a">
  <kap><tld/>a</kap>
  <snc>
    <ref tip="dif" cel="kresk.plen0a">plenkreska</ref>
    <ekz>
      li <klr>[...]</klr> postlasis &scirc;in, forirante al <tld/>a vivo 
en la
      urbo
      <fnt><aut>Gerrit Berveling</aut>: <vrk>Iom tro predikema romano
      (Lev Tolstoj, Resurekto)</vrk>, <bib>Monato</bib>, <lok>jaro
      2001a, numero 5a, p. 30a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_r;&c_o;&c_s;&c_l;&c_y;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_l;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>erwachsen</trd>,
    <trd>vollj&auml;hrig</trd>
  </trdgrp>
  <trd lng="es">adulto</trd>
  <trd lng="fr">adulte<klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="nl">
    <trd>volwassen</trd>,
    <trd>volgroeid</trd>
  </trdgrp>
  <trd lng="pl">doros&lstroke;y</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_z;&c_r;&c_o;&c_s;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_z;&c_r;&c_je;&c_l;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="adolt.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Stato de <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trdgrp lng="fr">
    <trd>&acirc;ge <ind>adulte</ind></trd>,
    <trd>maturit&eacute;<klr tip="ind"> (&acirc;ge adulte)</klr></trd>
  </trdgrp>
  <trd lng="nl">volwassenheid</trd>
  <trd lng="pl">doros&lstroke;o&sacute;&cacute;</trd>
  <trd lng="ru">&c_z;&c_r;&c_je;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="adolt.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Ekatingi la a&gcirc;on, la kapablojn de <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_l;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_t;&c_a;&c_l;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>erwachsen werden</trd>,
    <trd>heranwachsen</trd>,
    <trd>vollj&auml;hrig werden</trd>
  </trdgrp>
  <trd lng="es">ser adulto</trd>
  <trdgrp lng="fr">
    <trd>devenir <ind>adulte</ind></trd>,
    <trd>grandir<klr tip="ind"> (devenir adulte)</klr></trd>,
    <trd>m&ucirc;rir<klr tip="ind"> (devenir adulte)</klr></trd>
  </trdgrp>
  <trd lng="pl">dorosn&aogonek;&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_z;&c_r;&c_o;&c_s;&c_l;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_v;&c_z;&c_r;&c_o;&c_s;&c_l;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="adolt.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc>
    <dif>
      Ago <tld/>i&gcirc;i.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="fr">
    <trd>croissance<klr tip="ind"> (devenir adulte)</klr></trd>,
    <trd>maturation<klr tip="ind"> (devenir adulte)</klr></trd>
  </trdgrp>
  <trd lng="nl">het <ind>volwassen</ind> worden</trd>
  <trd lng="pl">dorastanie</trd>
  <trd lng="ru">&c_v;&c_z;&c_r;&c_o;&c_s;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>
</art>
