<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: balte.xml,v 1.18 2022/09/23 07:49:09 revo Exp $">
<kap>
  <rad>balte</rad>/o
  <fnt>V</fnt>
</kap>
<drv mrk="balte.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Rubando, <ref tip="super" cel="rimen.0o">rimeno</ref>
      el ledo a&ubreve; &scirc;tofo sur&scirc;ultre a&ubreve; trabruste portata, servanta por 
      subteni glavon, sakon a&ubreve; alian peza&jcirc;on:
      <ekz>
        la flago de la regiono Alzaco, prenis la blankan maldekstran balteon sur ru&gcirc;a
        fono kaj la florajn bendetojn de la flago de Malsupra Alzaco
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=511671">Malsupra Alzaco</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        grimpante sur la bran&ccirc;oj necesas sekurmaterialo kiel <tld/>o, &scirc;nuroj, nodoj,
        pulioj, ktp
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=239729">Arbogrimpado</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_t;&c_u;&c_p;&c_e;&c_ja;</trd>,
    <trd>&c_t;&c_a;&c_l;&c_mol;&c_m;&c_a;&c_h;</trd>,
    <trd>&c_v;&c_ja;&c_z;&c_mol;&c_m;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>balteus</trd>,
    <trd>baltheus</trd>,
    <trd>p&aacute;s p&rcaron;es rameno</trd>,
    <trd>svinut&yacute; p&aacute;s vymezen&yacute; volutami j&oacute;nsk&eacute; hlavice</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schulterriemen</trd>,
    <trd>Degengeh&auml;nge</trd>,
    <trd>Wehrgeh&auml;nge</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>baldrick</trd>,
    <trd>shoulder belt</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bandouli&egrave;re</trd>,
    <trd>baudrier</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&#x5D7;&#x5D2;&#x5D5;&#x5E8;&#x5D4;</trd>,
    <trd>&#x5D0;&#x5D6;&#x5D5;&#x5E8;</trd>
  </trdgrp>
  <trd lng="hu">v&aacute;llsz&iacute;j</trd>
  <trd lng="nl">schouderriem</trd>
  <trd lng="pl">pendent</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_r;&c_t;&c_u;&c_p;&c_je;&c_ja;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_ja;&c_z;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>n&aacute;plecn&yacute; nosn&yacute; reme&ncaron;</trd>,
    <trd>p&aacute;s alebo stuha</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&c_r;&c_t;&c_u;&#x43F;&c_je;&#x44F;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_je;&c_v;&#8217;&#x44F;&c_z;</trd>
  </trdgrp>
</drv>
</art>
