<kap>
  <rad>tricikl</rad>/o
<fnt><bib>JED</bib></fnt></kap>
<drv mrk="tricik.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Trirada <ref tip="lst" lst="voko:veturiloj" cel="veloci.0o">velocipedo</ref>:
      <ekz>
        la gara&gcirc;o de la <tld/>oj &scirc;irmis bovinojn kaj la dormejo de la <ref cel="rikisx.0o">riki&scirc;</ref>istoj 
        tenis fojnon kaj pajlon
        <fnt>
          <bib>MD05</bib>
          <vrk>La klimataj rifu&gcirc;intoj de Banglade&scirc;o</vrk>
        </fnt>;
      </ekz>
      <ekz>
        infanoj cirkulas sur plastaj <tld/>oj en la parkoj
        <fnt>
          <bib>MD17</bib>
          <vrk>Tiu sendependista regiono kiun rekonas nek Ukrainujo nek Rusujo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Ettore Bugatti</nom> <klr>[&#8230;]</klr> konstruis jam en la a&gcirc;o de 17 sian unuan motorizitan <tld/>on
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=57064">Ettore Bugatti</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="bicikl.0o">biciklo</ref>
    </refgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>triradilo</ke>, <ke>trirad(a) biciklo</ke>.
  </rim>

  <trd lng="be">&c_t;&c_r;&c_y;&c_c;&c_y;&c_k;&c_l;</trd>
  <trd lng="ca">tricicle</trd>
  <trdgrp lng="cs">
    <trd>tricykl</trd>,
    <trd>trojkolka</trd>,
    <trd>t&rcaron;&iacute;kolka</trd>
  </trdgrp>
  <trd lng="de">Dreirad</trd>
  <trd lng="en">trike</trd>
  <trd lng="es">triciclo</trd>
  <trd lng="fr">tricycle</trd>
  <trd lng="fa">&a_s;&a_h;&zwnj;&f_ch;&a_r;&a_kh;&a_h;</trd>
  <trdgrp lng="hu">
    <trd>tricikli</trd>,
    <trd>h&aacute;romkerek&udblac; <ind>ker&eacute;kp&aacute;r</ind></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#19977;&#36650;&#36554;  <pr>&#12373;&#12435;&#12426;&#12435;&#12375;&#12419;</pr></trd>,
    <trd>&#19977;&#36650;&#33258;&#36578;&#36554;  <pr>&#12415;&#12431;&#12376;&#12390;&#12435;&#12375;&#12419;</pr></trd>
  </trdgrp>
  <trd lng="nl">driewieler</trd>
  <trdgrp lng="pl">
    <trd>tr&oacute;jko&lstroke;owiec</trd>,
    <trd>tricykl</trd>
  </trdgrp>
  <trd lng="pt">triciclo</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_jo;&c_h;&c_k;&c_o;&c_l;&c_jo;&c_s;&c_n;&c_y;&c_j; &c_v;&c_je;&c_l;&c_o;&c_s;&c_i;&c_p;&c_je;&c_d;</trd>,
    <trd>&c_t;&c_r;&c_i;&c_c;&c_i;&c_k;&c_l;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>trojkolesov&yacute; bicykel</trd>,
    <trd>trojkolka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#1090;&#1088;&#1080;&#1094;&#1080;&#1082;&#1083;</trd>,
    <trd>&#1090;&#1088;&#1080;&#1082;&#1086;&#1083;&#1110;&#1089;&#1085;&#1080;&#1081; &#1074;&#1077;&#1083;&#1086;&#1089;&#1080;&#1087;&#1077;&#1076;/&#1072;&#1074;&#1090;&#1086;&#1084;&#1086;&#1073;&#1110;&#1083;&#1100;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#19977;&#36650;&#36554; <pr>s&#257;nl&#250;nch&#275;</pr></trd>,
    <trd>&#19977;&#36718;&#36710; <pr>s&#257;nl&#250;nch&#275;</pr></trd>
  </trdgrp>
</drv>
