<art mrk="$Id: kazuis.xml,v 1.19 2023/10/18 14:48:10 revo Exp $">
<kap>
  <rad>kazuist</rad>/o
</kap>
<drv mrk="kazuis.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">REL</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="teolog1.0isto">Teologiisto</ref> kiu studas la solvojn de konsciencaj duboj:
      <ekz>
        dek jarojn post la morto de <nom>Escobar</nom>, papo Inocento la XIa indeksis
        kelkajn skribojn de li kaj de aliaj <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_z;&c_u;&c_ib;&c_s;&c_t;</trd>
  <trd lng="ca">casuista</trd>
  <trdgrp lng="cs">
    <trd>kazuista</trd>,
    <trd>pochybova&ccaron;</trd>
  </trdgrp>
  <trd lng="de">Kasuist</trd>
  <trd lng="en">causist</trd>
  <trd lng="es">casuista</trd>
  <trd lng="fr">casuiste</trd>
  <trd lng="hu">kazuista</trd>
  <trd lng="ja">&#x6C7A;&#x7591;&#x8AD6;&#x8005;  <pr>&#x3051;&#x3064;&#x304E;&#x308D;&#x3093;&#x3057;&#x3083;</pr></trd>
  <trd lng="nl">Casu&iuml;st</trd>
  <trd lng="pl">kazuista</trd>     
  <trd lng="ro">cazuist</trd>
  <trd lng="ru">&c_k;&c_a;&c_z;&c_u;&c_i;&c_s;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>kazuista</trd>,
    <trd>pochybova&ccaron;</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&#x430;&#x437;&#x443;&#x457;&#x441;&#x442;</trd>
</drv>
</art>

