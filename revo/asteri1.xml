<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>asterisk</rad>/o
</kap>

<drv mrk="asteri1.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">TIP</uzo>
    <dif>
      <ref tip="dif" cel="interp3.0o">Interpunkcia</ref> <ref tip="dif"
      cel="sign.0o.KOMP">signo</ref> (*) kun formo de steleto, uzata en
      presarto por
      signali ion specialan, ekz. por: 
    </dif>
    <subsnc>
    <dif>
    atentigi pri piednoto,
    </dif>
    </subsnc>
    <subsnc>
    <dif>
    signi ian vortan kiel neekzistantan en la lingvo, kiam &gcirc;i estas
    metita tuj anta&ucirc; kaj post tiun vorton.
    </dif>
    </subsnc>
  </snc>
  <trd lng="be">&c_z;&c_o;&c_r;&c_a;&c_ch;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>asterisk</trd>,
    <trd>znak hv&ecaron;zdi&ccaron;ka <klr>(typogr.)</klr> *</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Asterisk</trd>,
    <trd>Sternchen</trd>
  </trdgrp>
  <trd lng="en">asterisk</trd>
  <trd lng="es">asterisco</trd>
  <trd lng="fr">ast&eacute;risque</trd>
  <trdgrp lng="hu">
    <trd>aszteriszk</trd>,
    <trd>csillagjel</trd>
  </trdgrp>
  <trd lng="it">asterisco</trd>
  <trdgrp lng="nl">
    <trd>asterisk</trd>,
    <trd>sterretje</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>odsy&lstroke;acz</trd>,
    <trd>asterysk</trd>
  </trdgrp>
  <trd lng="ro">asterisc</trd>
  <trd lng="ru">&c_z;&c_v;&c_jo;&c_z;&c_d;&c_o;&c_ch;&c_k;&c_a;</trd>
  <trd lng="sk">hviezdi&ccaron;ka *</trd>
</drv>
</art>
