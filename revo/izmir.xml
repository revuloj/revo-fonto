<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: izmir.xml,v 1.6 2023/11/19 10:22:14 afido Exp $">
    <kap>
      <rad>Izmir</rad>/o
    </kap>

<drv mrk="izmir.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Granda <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      en la oriento de <ref tip="malprt" cel="turk.T0ujo">Turkujo</ref>
      &ccirc;e la Egea Maro, sudoriente de la insulo <ref cel="lesb.0o">Lesbo</ref>:
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> estas anka&ubreve; nomata &leftquot;Perlo de la Egea Maro&rightquot;, dum la
        antikva epoko la urbo havis la grekan nomon <nac>Smyrna</nac> (Smirno)
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Izmiro">Izmiro</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la sekvan tagon ni da&ubreve;rigis nian voja&gcirc;on direkte al <tld/>o
        <fnt>
          <aut>Herbert A. Welker</aut>
          <vrk><url ref="http://esperantaretradio.blogspot.com/2018/07/alloga-turkujo.html">Alloga
          Turkujo</url></vrk>
          <lok>esperanta retradio</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_Ib;&c_z;&c_mol;&c_m;&c_ib;&c_r;</trd>
    <trd lng="de">Izmir</trd>
    <trd lng="el">&Sigma;&my;&ypsilon_ton;&rho;&ny;&eta;</trd>
    <trd lng="en">&Idot;zmir</trd>
    <trd lng="fr">Izmir</trd>
    <trd lng="grc">&Sigma;&my;&ypsilon_ton;&rho;&ny;&alfa;</trd>
    <trd lng="tr">&Idot;zmir</trd>
  </snc>
  <trd lng="ja">&#x30A4;&#x30BA;&#x30DF;&#x30FC;&#x30EB;</trd>
  <trd lng="uk">&#x406;&#x437;&#x43C;&#x456;&#x440;</trd>
  <trd lng="zh">&#x4F0A;&#x5179;&#x5BC6;&#x5C14; <pr>y&#x12B;c&#xED;m&#xEC;&#x11B;r</pr></trd>
</drv>

  </art>

