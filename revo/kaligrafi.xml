<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kaligrafi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kaligrafi.0o">
  <kap><tld/>o</kap>
  <snc mrk="kaligrafi.0o.BELA">
  <uzo tip="fak">BELA</uzo>
    <ref tip="dif" cel="skrib.bel0ado.BELA">belskribado</ref>
    <ekz>
      en <tld/>o kaj la rusa lingvo mi neniam povis vin atingi
      <fnt><bib>Metrop</bib></fnt>.
    </ekz>
  </snc>
  <trd lng="fr">calligraphie</trd>
  <trdgrp lng="hu">
    <trd>kalligr&aacute;fia</trd>,
    <trd>sz&eacute;p&iacute;r&aacute;s</trd>
  </trdgrp>
</drv>

<drv mrk="kaligrafi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="kaligrafi.0isto.BELA">
  <uzo tip="fak">BELA</uzo>
    <ref tip="dif" cel="skrib.bel0isto.BELA">belskribisto</ref>
  </snc>
  <trd lng="fr">calligraphe</trd>
  <trd lng="hu">kalligr&aacute;fia</trd>
</drv>
</art>

