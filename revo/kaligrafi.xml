<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kaligrafi.xml,v 1.15 2022/09/14 09:36:39 revo Exp $">
<kap>
  <rad>kaligrafi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="kaligrafi.0o">
  <kap><tld/>o</kap>
  <snc mrk="kaligrafi.0o.BELA">
  <uzo tip="fak">BELA</uzo>
    <dif>
      <ref tip="dif" cel="skrib.bel0ado.BELA">Belskribado</ref>:
      <ekz>
        en <tld/>o kaj la rusa lingvo mi neniam povis vin atingi
        <fnt><bib>Metrop</bib></fnt>;
      </ekz>
      <ekz>
        programo por lerni la &ccirc;inan <tld/>on
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato1/monato-9506-lingvema.txt">Por lingvemaj
          komputilemuloj...</url></vrk>
          <lok>95/06</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la ekspozicio oni rigardas envie <klr>[&#8230;]</klr> pentra&jcirc;ojn,
        ceramika&jcirc;ojn,
        <tld/>a&jcirc;ojn
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Monato;/2004/008393.html">Alproprigi hobion</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_ib;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>
  <trd lng="bo">&#xF61;&#xF72;&#xF42;&#xF0B;&#xF42;&#xF5F;&#xF74;&#xF42;&#xF66;&#xF0B;</trd>
  <trd lng="ca">cal&middot;ligrafia</trd>
  <trd lng="cs">krasopis</trd>
  <trd lng="de">Kalligrafie</trd>
  <trd lng="fr">calligraphie</trd>
  <trdgrp lng="hu">
    <trd>kalligr&aacute;fia</trd>,
    <trd>sz&eacute;p&iacute;r&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kaligrafia</trd>,
    <trd>pismo pi&eogonek;kne</trd>
  </trdgrp>  
  <trd lng="pt">caligrafia</trd>
  <trd lng="ro">caligrafie</trd>
  <trd lng="sk">krasopis</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_a;&#x43B;&c_ib;&c_g;&c_r;&c_a;&c_f;&c_ib;&#x44F;</trd>,
    <trd>&#x43A;&c_r;&c_a;&c_s;&#x43D;&#x43E;&#x43F;&c_i;&c_s;</trd>
  </trdgrp>
</drv>

<drv mrk="kaligrafi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="kaligrafi.0isto.BELA">
  <uzo tip="fak">BELA</uzo>
    <dif>
      <ref tip="dif" cel="skrib.bel0isto.BELA">Belskribisto</ref>:
      <ekz>
        li studis en pluraj artlernejoj en Parizo kaj i&gcirc;is sendependa
        <tld/>isto
        <fnt>
          <vrk><url ref="https://peoplepill.com/people/jean-larcher/">Jean Larcher - Biography,
          Life, Family, Career, Facts
              ...</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;efaj iloj por <tld/>isto estas la plumo <klr>[&#8230;]</klr> kaj
        la broso
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19128">Skribadarto</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_ib;&c_g;&c_r;&c_a;&c_f;</trd>
  <trd lng="ca">cal&middot;l&iacute;graf</trd>
  <trd lng="de">Kalligraf</trd>
  <trd lng="fr">calligraphe</trd>
  <trd lng="hu">kalligr&aacute;fia</trd>
  <trd lng="pl">kaligraf</trd>
  <trd lng="pt">cal&iacute;grafo</trd>
  <trd lng="ro">caligraf</trd>
</drv>
</art>

