<art mrk="$Id: redakc.xml,v 1.24 2024/04/13 20:15:52 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>redakci</rad>/o
</kap>
<drv mrk="redakc.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="redakc.0o.istaro">
    <dif>
      Tuto de tiuj, kiuj redaktas gazeton; redaktistaro:
      <ekz>
        &scirc;an&gcirc;o en la Esperanto-<tld/>o de la Pola Radio.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Redaktionsteam</trd>,
      <trd>Redakteure</trd>
    </trdgrp>
    <trd lng="en">editorial staff</trd>
    <trd lng="fr">r&eacute;daction<klr> (&eacute;quipe de)</klr></trd>
    <trd lng="hu">szerkeszt&odblac;s&eacute;g</trd>
  </snc>
  <snc mrk="redakc.0o.ejo">
    <dif>
      <ref tip="dif" cel="redakt.0ejo">Redaktejo</ref>.
    </dif>
    <trdgrp lng="de">
      <trd>Redaktionsb&uuml;ro</trd>
    </trdgrp>
    <trd lng="en">editorial office</trd>
    <trd lng="fr">r&eacute;daction<klr> (bureau de)</klr></trd>
    <trd lng="hu">szerkeszt&odblac;s&eacute;g</trd>
    <trdgrp lng="zh">
    <trd>&#x7DE8;&#x8F2F;&#x5BA4; <pr>bi&#x101;nj&#xED;sh&#xEC;</pr></trd>,
    <trd>&#x7F16;&#x8F91;&#x5BA4; <pr>bi&#x101;nj&#xED;sh&#xEC;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="redakc.0o.versio">
    <dif>
      Stato de teksto, programo ktp:
      <ekz>
        ilia definitiva <tld/>o estos fiksita nur en la
        kongreso
        <fnt><bib>DdH</bib>: <lok>RE, 1906:2, p.27.</lok></fnt>;
      </ekz>
      <ekz>
        la redakcio 24-a de la <ctl>Komputada leksikono</ctl>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="leg.0o.versio">lego</ref>,
      <ref cel="varian.0o">varianto</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Redaktionsstand</trd>,
      <trd>redaktioneller Bearbeitungsstand</trd>
    </trdgrp>
    <trd lng="nl">versie</trd>
    <trd lng="en">edition</trd>
    <trd lng="fr">r&eacute;daction<klr> (d'un texte)</klr></trd>
    <trd lng="hu">szerkeszt&eacute;s</trd>
    <trd lng="zh">&#x7248;&#x672C; <pr>b&#x1CE;nb&#x11B;n</pr></trd>
</snc>
  <trd lng="be">&c_r;&c_e;&c_d;&c_a;&c_k;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>modifikace</trd>,
    <trd>redigov&#xE1;n&#xED;</trd>,
    <trd>upravov&#xE1;n&#xED;</trd>
  </trdgrp>
  <trd lng="de">Redaktion</trd>
  <trd lng="ja">&#x7DE8;&#x96C6;&#x90E8;  <pr>&#x3078;&#x3093;&#x3057;&#x3085;&#x3046;&#x3076;</pr></trd>
  <trd lng="nl">redactie</trd>
  <trdgrp lng="pl">
    <trd>redakcja</trd>,
    <trd>wydawnictwo</trd>
  </trdgrp>
  <trd lng="ro">redac&#x21B;ie</trd>
  <trd lng="ru">&c_r;&c_je;&c_d;&c_a;&c_k;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>modifik&#xE1;cia</trd>,
    <trd>redigovanie</trd>,
    <trd>upravovanie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_je;&c_d;&c_a;&#x43A;&c_c;&c_ib;&#x44F;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_s;&c_ib;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
