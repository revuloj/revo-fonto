  <trd lng="nl">redactie</trd>
  <trdgrp lng="pl">
    <trd>redakcja</trd>,
    <trd>wydawnictwo</trd>
  </trdgrp>
  <trd lng="ro">redac&#539;ie</trd>
  <trd lng="ru">&c_r;&c_je;&c_d;&c_a;&c_k;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>modifik&#225;cia</trd>,
    <trd>redigovanie</trd>,
    <trd>upravovanie</trd>
  </trdgrp>
  <trdgrp lng="uk">
