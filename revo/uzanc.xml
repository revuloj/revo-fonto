<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>uzanc</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="uzanc.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
    <snc mrk="uzanc.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="tradic.0o.ajxo">Tradicio</ref>, agmaniero kutima
kaj normala en iu homgrupo:
      <ekz>
        li agis tute laŭ spirito de la tiamaj komercaj kaj maraj <tld/>oj
        <fnt>
          <bib>Salamandroj</bib>,
          <lok>libro 3a, 1, „Masakro sur Kokosa Insularo“</lok>
        </fnt>;
      </ekz>
      <ekz>
        demokratiaj reformoj forbalaas koruptajn <tld/>ojn
        <fnt>
          <aut>Ĝangalo</aut>,
          <vrk><url
ref="http://gxangalo.com/modules/news/article.php?storyid=1805">Vangaria
Maathai, Nobel-premiita pri Paco 2004</url></vrk>,
          <lok>2004-11-04</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>us<klr> (coutumes)</klr></trd>,
    <trd>usages<klr> (coutumes)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>obyczaj</trd>
    <trd>zwyczaj</trd>
    <trd>praktyka</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>usança</trd>,
    <trd>costume velho</trd>,    
    <trd>hábito tradicional</trd>,    
    <trd>praxe</trd>,
    <trd>uso freqüente</trd>    
  </trdgrp>
</drv>
</art>

