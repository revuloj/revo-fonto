<art mrk="$Id: uzanc.xml,v 1.15 2022/03/28 11:08:54 revo Exp $">
<kap>
<rad>uzanc</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="uzanc.0o">
   <kap><tld/>o</kap>
   <uzo tip="stl">RAR</uzo>
   <snc mrk="uzanc.0o.MOR">
     <dif>     
       <ref tip="dif" cel="kutim.0o.MOR">Kutimo<sncref/></ref>:
       <ekz>
         li agis tute la&ubreve; spirito de la tiamaj komercaj kaj maraj
         <tld/>oj
         <fnt>
           <bib>Salamandroj</bib>,
           <lok>libro 3a, 1, &leftquot;Masakro sur Kokosa
           Insularo&rightquot;</lok>
         </fnt>;
       </ekz>
       <ekz>
         demokratiaj reformoj forbalaas koruptajn <tld/>ojn
         <fnt>
           <aut>&Gcirc;angalo</aut>,
           <vrk><url ref="http://gxangalo.com/modules/news/article.php?storyid=1805">
           Vangaria Maathai, Nobel-premiita pri Paco 2004</url></vrk>,
           <lok>2004-11-04</lok>
         </fnt>.
       </ekz>
     </dif>
   </snc>
  <trdgrp lng="cs">
    <trd>obchodn&iacute; zvyklosti</trd>,
    <trd>uzance</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Brauch</trd>,
    <trd>Gebrauch <klr>(Tradition)</klr></trd>,
    <trd>Gewohnheit</trd>,
    <trd>Usus</trd>
  </trdgrp>
  <trd lng="en">custom</trd>
  <trdgrp lng="fi">
     <trd>k&auml;yt&auml;nne</trd>,
     <trd>menettelytapa</trd>
   </trdgrp>
  <trdgrp lng="fr">
     <trd>us<klr> (coutumes)</klr></trd>,
     <trd>usages<klr> (coutumes)</klr></trd>
   </trdgrp>
  <trdgrp lng="it">
     <trd>usanza<klr> (abitudine)</klr></trd>,
     <trd>costume<klr> (usanza)</klr></trd>
   </trdgrp>
  <trdgrp lng="pl">
     <trd>obyczaj</trd>
     <trd>zwyczaj</trd>
     <trd>praktyka</trd>
   </trdgrp>
  <trdgrp lng="pt">
     <trd>usan&ccedil;a</trd>,
     <trd>costume velho</trd>,
     <trd>h&aacute;bito tradicional</trd>,
     <trd>praxe</trd>,
     <trd>uso freq&uuml;ente</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>zvyklos&tcaron;</trd>,
    <trd>&uacute;zus</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x442;&#x43E;&#x440;&#x433;&#x43E;&#x432;&#x456; &#x437;&#x432;&#x438;&#x447;&#x430;&#x457;</trd>,
    <trd>&#x43A;&#x43E;&#x43C;&#x435;&#x440;&#x446;&#x456;&#x439;&#x43D;&#x430; &#x43F;&#x440;&#x430;&#x43A;&#x442;&#x438;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>
</art>
