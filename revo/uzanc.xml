<kap>
<rad>uzanc</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="uzanc.0o">
   <kap><tld/>o</kap>
   <uzo tip="stl">RAR</uzo>
   <snc mrk="uzanc.0o.MOR">
     <dif>     
       <ref tip="dif" cel="kutim.0o.MOR">Kutimo<sncref/></ref>:
       <ekz>
         li agis tute la&ubreve; spirito de la tiamaj komercaj kaj maraj
         <tld/>oj
         <fnt>
           <bib>Salamandroj</bib>,
           <lok>libro 3a, 1, &leftquot;Masakro sur Kokosa
           Insularo&rightquot;</lok>
         </fnt>;
       </ekz>
       <ekz>
         demokratiaj reformoj forbalaas koruptajn <tld/>ojn
         <fnt>
           <aut>&Gcirc;angalo</aut>,
           <vrk><url ref="http://gxangalo.com/modules/news/article.php?storyid=1805">
           Vangaria Maathai, Nobel-premiita pri Paco 2004</url></vrk>,
           <lok>2004-11-04</lok>
         </fnt>.
       </ekz>
     </dif>
   </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>uzo</ke>, <ke>uzado</ke>, <ke>uzkutimo</ke>.
  </rim>

   <trdgrp lng="be">
     <trd>&c_z;&c_v;&c_y;&c_ch;&c_a;&c_j;</trd>,
     <trd>&c_z;&c_a;&c_v;&c_ja;&c_d;&c_z;&c_jo;&c_n;&c_k;&c_a;</trd>,
     <trd>&c_p;&c_r;&c_a;&c_k;&c_t;&c_y;&c_k;&c_a; <klr tip="amb">(&c_ja;&c_k;&c_a;&c_ja; &c_s;&c_k;&c_l;&c_a;&c_l;&c_a;&c_s;&c_ja;)</klr></trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>obchodn&iacute; zvyklosti</trd>,
    <trd>uzance</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Brauch</trd>,
    <trd>Gebrauch <klr>(Tradition)</klr></trd>,
    <trd>Gewohnheit</trd>,
    <trd>Usus</trd>
  </trdgrp>
  <trd lng="en">custom</trd>
  <trdgrp lng="fi">
     <trd>k&auml;yt&auml;nne</trd>,
     <trd>menettelytapa</trd>
   </trdgrp>
  <trdgrp lng="fr">
     <trd>us<klr> (coutumes)</klr></trd>,
     <trd>usages<klr> (coutumes)</klr></trd>
   </trdgrp>
  <trdgrp lng="it">
     <trd>usanza<klr> (abitudine)</klr></trd>,
     <trd>costume<klr> (usanza)</klr></trd>
   </trdgrp>
  <trdgrp lng="pl">
     <trd>obyczaj</trd>
     <trd>zwyczaj</trd>
     <trd>praktyka</trd>
   </trdgrp>
  <trdgrp lng="pt">
     <trd>usan&ccedil;a</trd>,
     <trd>costume velho</trd>,
     <trd>h&aacute;bito tradicional</trd>,
     <trd>praxe</trd>,
     <trd>uso freq&uuml;ente</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>zvyklos&tcaron;</trd>,
    <trd>&uacute;zus</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&#1086;&c_r;&c_g;&#1086;&c_v;&c_ib; &c_z;&c_v;&c_i;&c_ch;&c_a;&c_ji;</trd>,
    <trd>&#1082;&#1086;&#1084;&c_je;&c_r;&c_c;&c_ib;&c_j;&#1085;&c_a; &#1087;&c_r;&c_a;&#1082;&c_t;&c_i;&#1082;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20064;&#20439; <pr>x&#237;s&#250;</pr></trd>,
    <trd>&#32722;&#20439; <pr>x&#237;s&#250;</pr></trd>,
    <trd>&#39080;&#20439; <pr>f&#275;ngs&#250;</pr></trd>,
    <trd>&#39118;&#20439; <pr>f&#275;ngs&#250;</pr></trd>,
    <trd>&#20439; <pr>s&#250;</pr></trd>,
    <trd>&#24815;&#20363; <pr>gu&#224;nl&#236;</pr></trd>,
    <trd>&#24931;&#20363; <pr>gu&#224;nl&#236;</pr></trd>,
    <trd>&#39080;&#27683; <pr>f&#275;ngq&#236;</pr></trd>,
    <trd>&#39118;&#27668; <pr>f&#275;ngq&#236;</pr></trd>
  </trdgrp>
</drv>
