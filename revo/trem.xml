<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: trem.xml,v 1.40 2024/11/05 15:32:08 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>trem</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="trem.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="trem.0i.KOMUNE">
    <dif>
      Esti movata tien kaj reen per malgrandaj sinsekvaj skuetoj:
      <ekz>
        ili timu vin kaj <tld/>u anta&ubreve; vi
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 9:2</lok>
        </fnt>;
      </ekz><ekz>
        a&ubreve;di&gcirc;is tre forta sonado de korno kaj ek<tld/>is la tuta popolo
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 19:16</lok>
        </fnt>;
      </ekz><ekz>
        timema kaj <tld/>ema
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 7:3</lok>
        </fnt>;
      </ekz><ekz>
        mia koro <tld/>egas, forlasis min mia forto
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 38:10</lok>
        </fnt>;
      </ekz><ekz>
        &scirc;iaj lipoj <tld/>is, &scirc;iaj okuloj vuali&gcirc;is de larmoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz><ekz>
        &scirc;i <klr>[&#8230;]</klr> legis plu, sed per vo&ccirc;o &ccirc;iam ankora&ubreve; <tld/>anta, kvankam &scirc;i
        penis kovri sian eksciton
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz><ekz>
        la arboj &jcirc;etadis belajn kaj <tld/>antajn ombrojn sur la herbon
        <fnt><bib>BdV</bib></fnt>;
      </ekz><ekz>
        sur <klr>[&#8230;]</klr> folioj pluvaj gutoj <tld/>is kaj fajrerante defaladis
        <fnt><bib>BdV</bib></fnt>;
      </ekz><ekz>
        blinda maljunulo <klr>[&#8230;]</klr> kun <tld/>antaj manoj ser&ccirc;ante
        rampis la&ubreve;longe la muro
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
        </fnt>;
      </ekz><ekz>
        mi sentis ke la tero <tld/>as sub miaj piedoj <klr>[&#8230;]</klr> kaj mi vidis ke,
        <klr>[&#8230;]</klr> kavi&gcirc;as profundega truo, en kiun la domo malaperis
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz><ekz>
        la ma&scirc;ino kun <ind><tld/>iganta</ind> rapideco kuregis
        <klr>[&#8230;]</klr> kaj subite akre &jcirc;etadis sin flanken
        <fnt><bib>Metrop</bib></fnt>;
        <trd lng="de">schaurig</trd>
        <trd lng="hu">megrend&iacute;t&odblac;</trd>
        <trd lng="ru">&c_p;&c_r;&c_i;&c_v;&c_o;&c_d;&c_ja;&c_shch;&c_i;&c_j; &c_v;
        <ind>&c_t;&c_r;&c_je;&c_p;&c_je;&c_t;</ind></trd>
      </ekz><ekz>
        pli bone estas &scirc;viti ol <tld/>i
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz><ekz>
        <ind>li <tld/>as, kiel a&ubreve;tuna folio</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
        <trd lng="hu">remeg, mint a ny&aacute;rfalev&eacute;l</trd>
      </ekz><ekz>
        oni tondas &scirc;afinojn, <tld/>as la &scirc;afoj
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="agit.0igxi">agiti&gcirc;i</ref>,
      <ref cel="febr.0i.MED">febri<sncref/></ref>,
      <ref cel="jxet.0igxadi">&jcirc;eti&gcirc;adi</ref>,
      <ref cel="konvul.0i">konvulsii</ref>,
      <ref cel="sku.0igxi">skui&gcirc;i</ref>,
      <ref cel="spasm.0i">spasmi</ref>,
      <ref cel="sxance.0igxi.MOV">&scirc;anceli&gcirc;i<sncref/></ref>,
      <ref cel="vibr.0i.tremmove">vibri<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_r;&c_y;&c_zh;&c_e;&c_c;&c_mol;</trd>,
    <trd>&c_t;&c_r;&c_y;&c_m;&c_c;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_t;&c_r;&c_y;&c_m;&c_c;&c_je;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">tremolar</trd>
  <trdgrp lng="cs">
    <trd>chv&ecaron;t se</trd>,
    <trd>tetelit se</trd>,
    <trd>zachv&iacute;vat se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>zittern</trd>,
    <trd>beben</trd>,
    <trd>flattern</trd>
  </trdgrp>
  <trd lng="el">&tau;&rho;&epsilon_ton;&my;&omega;</trd>
  <trdgrp lng="en">
    <trd>tremble</trd>,
    <trd>shiver</trd>,
    <trd>quiver</trd>
  </trdgrp>
  <trd lng="es">temblar</trd>
  <trd lng="fr">trembler</trd>
  <trdgrp lng="hu">
    <trd>remeg</trd>,
    <trd>reszket</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#38663;&#12360;&#12427;  <pr>&#12405;&#12427;&#12360;&#12427;</pr></trd>,
    <trd>&#36523;&#38663;&#12356;&#12377;&#12427;  <pr>&#12415;&#12406;&#12427;&#12356;&#12377;&#12427;</pr></trd>,
    <trd>&#25594;&#12428;&#12427;  <pr>&#12422;&#12428;&#12427;</pr></trd>,
    <trd>&#25594;&#12428;&#21205;&#12367;  <pr>&#12422;&#12428;&#12358;&#12372;&#12367;</pr></trd>,
    <trd>&#38663;&#21205;&#12377;&#12427;  <pr>&#12375;&#12435;&#12393;&#12358;&#12377;&#12427;</pr></trd>,
    <trd>&#24656;&#12428;&#12427;  <pr>&#12362;&#12381;&#12428;&#12427;</pr></trd>,
    <trd>&#12403;&#12367;&#12403;&#12367;&#12377;&#12427;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>beven</trd>,
    <trd>trillen</trd>,
    <trd>bibberen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>trz&aogonek;&sacute;&cacute; si&eogonek;</trd>,
    <trd>dygota&cacute;</trd>,
    <trd>drga&cacute;</trd>,
    <trd>dr&zdot;e&cacute;</trd>
  </trdgrp>
  <trd lng="pt">tremer</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_r;&c_o;&c_zh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_o;&c_d;&c_r;&c_o;&c_g;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_p;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chvie&tcaron; sa</trd>,
    <trd>zachvieva&tcaron; sa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_r;&c_je;&c_m;&c_t;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_d;&c_r;&c_i;&c_zh;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_t;&c_r;&c_u;&c_s;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_t;&c_r;&c_ja;&c_s;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_t;&c_r;&c_ib;&c_p;&c_o;&c_t;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i; &c_o;&c_z;&c_n;&c_o;&c_b;</trd>,
    <trd>&c_z;&c_d;&c_r;&c_i;&c_g;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21457;&#25238; <pr>f&amacron;d&#466;u</pr></trd>,
    <trd>&#30332;&#25238; <pr>f&amacron;d&#466;u</pr></trd>,
    <trd>&#38663;&#39019; <pr>zh&egrave;nch&agrave;n</pr></trd>,
    <trd>&#38663;&#39076; <pr>zh&egrave;nch&agrave;n</pr></trd>,
    <trd>&#21702;&#21990; <pr>du&omacron;suo</pr></trd>,
    <trd>&#25238; <pr>d&#466;u</pr></trd>,
    <trd>&#29791;&#32034; <pr>s&egrave;su&#466;</pr></trd>,
    <trd>&#25112;&#25238; <pr>zh&agrave;nd&#466;u</pr></trd>,
    <trd>&#25136;&#25238; <pr>zh&agrave;nd&#466;u</pr></trd>
  </trdgrp>
</drv>

<drv mrk="trem.0o">
  <kap><tld/>o, <var><kap><tld/>ado</kap></var></kap>
  <snc mrk="trem.0o.MOV">
    <dif>
      Movi&gcirc;o de io <tld/>anta:
      <ekz>
        atakis min teruro kaj <tld/>o kaj &ccirc;iuj miaj ostoj eksentis timon
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 4:14</lok>
        </fnt>;
      </ekz><ekz>
        <tld/>o, kiel &ccirc;e aku&scirc;antino
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 48:6</lok>
        </fnt>;
      </ekz><ekz>
        <ind>kor<tld/>o</ind>
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 28:67</lok>
        </fnt>;
        <trd lng="de">Herzflattern</trd>
      </ekz><ekz>
        ili forkuris de la tombo, &ccirc;ar <tld/>ado kaj konfuzi&gcirc;o posedis ilin
        <fnt>
          <bib>NT</bib>
          <lok>Marko 16:8</lok>
        </fnt>;
      </ekz><ekz>
        <tld/>o de morto
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz><ekz>
        &ccirc;e tiu &ccirc;i penso malvarma <tld/>o trakuris la korpon de Marta
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz><ekz>
        vana <tld/>ado de la patrina koro
        <fnt>
          <bib>Marta</bib>
        </fnt>!
      </ekz><ekz>
        adia&ubreve;! li diris kun <tld/>o en la vo&ccirc;o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz><ekz>
        en plej dol&ccirc;aj <tld/>oj tuj ekbatis homa koro
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz><ekz>
        la unuaj ek<tld/>oj de veki&gcirc;anta papilia amo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz><ekz>
        ekscita <tld/>o ruli&gcirc;is tra l' amaso dum atendo
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz><ekz>
        stranga <tld/>o trapenetris min kaj senmovigis
        <fnt>
          <bib>FdO</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vibr.0o.MOV">vibro</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_r;&c_y;&c_zh;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_d;&c_r;&c_y;&c_g;&c_o;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_d;&c_r;&c_y;&c_zh;&c_y;&c_k;&c_ib;</trd>,
    <trd>&c_t;&c_r;&c_y;&c_m;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="cs">chv&ecaron;n&iacute;</trd>
  <trd lng="es">vibraci&oacute;n</trd>
  <trd lng="fr">temblement</trd>
  <trdgrp lng="ja">
    <trd>&#38663;&#21205;  <pr>&#12375;&#12435;&#12393;&#12358;</pr></trd>,
    <trd>&#25594;&#12428;  <pr>&#12422;&#12428;</pr></trd>
  </trdgrp>
  <trd lng="nl">bibberen<klr tip="ind"> (zn.)</klr></trd>
  <trdgrp lng="pl">
    <trd>trz&eogonek;sienie</trd>,
    <trd>dygotanie</trd>,
    <trd>dr&zdot;enie</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_r;&c_o;&c_zh;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>trasenie</trd>,
    <trd>tria&scaron;ka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_r;&c_o;&c_zh;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_m;&c_t;&c_ib;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
  <trd lng="zh">&#21702; <pr>du&omacron;</pr></trd>
</drv>

<drv mrk="trem.ek0i">
  <kap>ek<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="trem.0i.komenci">
    <dif>
      Komenci <tld/>i:
      <ekz>
        la turmentata animo trarigardis spirite &ccirc;iujn agojn
        de sia tera vivo kaj ek<tld/>is sub larmoj tiel,
        kiel <nom>Inger</nom> neniam estis havinta la kapablon plori
        <fnt>
          <bib>Fab4</bib>,
          <lok>Knabino, kiu pa&scirc;is sur panon</lok>
        </fnt>;
      </ekz><ekz>
        atakis teruro, ek<tld/>is la kruro
	<fnt><bib>PrV</bib></fnt>;
      </ekz><ekz>
        la senkolori&gcirc;intaj pupiloj de la maljunulino ne
        malseki&gcirc;is per larmo kaj ne eklumis e&ccirc; per la plej
        malgranda brilo, kiam &scirc;i ekparolis la lastajn vortojn,
        sed &scirc;ia pala mallar&gcirc;a bu&scirc;o ek<tld/>is inter
        la amaso de la &gcirc;in &ccirc;irka&ubreve;antaj sulketoj kaj la kava
        brusto ek<tld/>is sub la antikvamoda manteleto
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>erzittern</trd>,
      <trd>erschauern</trd>,
      <trd>erbeben</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>dar repeluzno</trd>,
      <trd>tener escalofr&iacute;os</trd>
    </trdgrp>
    <trd lng="fr">frissonner</trd>
    <trd lng="hu">elkezd <ind>remegni</ind></trd>
    <trdgrp lng="nl">
      <trd>beginnen te <ind>beven</ind></trd>,
      <trd>beginnen te <ind>trillen</ind></trd>,
      <trd>beginnen te <ind>bibberen</ind></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>zatrz&aogonek;&sacute;&cacute; si&eogonek;</trd>,
      <trd>zadygota&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_d;&c_r;&c_o;&c_zh;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_t;&c_r;&c_je;&c_p;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="trem.0i.eri">
    <dif>
      <ref tip="dif" cel="trem.0eri"><tld lit="T"/>eri</ref>:
      <ekz>
          la birdo etendis siajn belajn flugilojn, ankora&ubreve; unu fojon
          konvulsie ek<tld/>is <klr>&#8230;</klr> kaj &gcirc;ia koro krevis de
          senforteco kaj sopirado
          <fnt><bib>Fab1</bib>, <lok>Lekanto</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="es">
      <trd>sobrecoger</trd>,
      <trd>sobresaltar</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>megremeg</trd>,
      <trd>&ouml;sszerezzen</trd>
    </trdgrp>
    <trd lng="nl">ineen<ind>krimpen</ind><klr tip="ind">
    (v.d.schrik)</klr></trd>
    <trdgrp lng="pl">
      <trd>zadrga&cacute;</trd>,
      <trd>zadr&zdot;e&cacute;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_z;&c_d;&c_r;&c_y;&c_g;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_r;&c_y;&c_zh;&c_e;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_r;&c_y;&c_m;&c_c;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>rozt&rcaron;&aacute;st se</trd>,
    <trd>zachv&ecaron;t se</trd>,
    <trd>zat&rcaron;&aacute;st se</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#36523;&#38663;&#12356;&#12377;&#12427;  <pr>&#12415;&#12406;&#12427;&#12356;&#12377;&#12427;</pr></trd>,
    <trd>&#12403;&#12367;&#12387;&#12392;&#12377;&#12427;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>strias&tcaron; sa</trd>,
    <trd>za&ccaron;a&tcaron; sa trias&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_t;&c_r;&c_je;&c_m;&c_t;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_r;&c_i;&c_zh;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_r;&c_ja;&c_s;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="trem.0eri">
  <kap><tld/>eri</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="trem.0eri.MOV">
    <dif>
      Fari unuopan <tld/>movon:
      <ekz>
        parolante, la gasto plurfoje angore <tld/>eris
        <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 13a</lok></fnt>;
      </ekz><ekz>
        sed se &scirc;i estis ekparolanta, do tiel subite kaj la&ubreve;te,
        ke oni <tld/>eris
        <fnt>
          <aut>I. Bunin, trad. G. Arosev</aut>, <vrk>Rusja</vrk>,
          <lok>en: Rusaj amnoveloj, 2000.</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="konvul.0i">konvulsii</ref>,
       <ref cel="tik.0i">tiki</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_z;&c_d;&c_r;&c_y;&c_g;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_l;&c_a;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_l;&c_a;&c_t;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_d;&c_r;&c_y;&c_g;&c_a;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>zucken</trd>,
    <trd>zusammenzucken</trd>,
    <trd>erschauern</trd>
  </trdgrp>
  <trd lng="en">to <ind>start</ind></trd>
  <trdgrp lng="es">
    <trd>sobrecoger</trd>,
    <trd>sobresaltar</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megremeg</trd>,
    <trd>&ouml;sszerezzen</trd>
  </trdgrp>
  <trd lng="fr">sursauter</trd>
   <trd lng="nl">ineen<ind>krimpen</ind><klr tip="ind">
   (v.d.schrik)</klr></trd>
  <trdgrp lng="pl">
    <trd>zatrz&aogonek;&sacute;&cacute; si&eogonek;</trd>,
    <trd>zadygota&cacute;</trd>,
    <trd>drgn&aogonek;&cacute;</trd>,
    <trd>zadr&zdot;e&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_r;&c_o;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>,
