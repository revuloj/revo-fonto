<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>trem</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="trem.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="trem.0i.KOMUNE">
    <dif>
      Esti movata tien kaj reen per malgrandaj sinsekvaj skuetoj:
      <ekz>
        la ponto <tld/>as kiam oni veturas sur &gcirc;i<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i pro frosto, pro timo;
      </ekz>
      <ekz>
        lia vo&ccirc;o, liaj lipoj <tld/>is<fnt>B</fnt>;
      </ekz>
      <ekz>
        <ind><tld/>i kiel a&ubreve;tuna folio</ind><fnt>Z</fnt>;
        <trd lng="hu">remeg, mint a ny&aacute;rfalev&eacute;l</trd>
      </ekz>
      <ekz>
        ek<tld/>is kaj ekskui&gcirc;is la tero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        pluvaj gutoj <tld/>is sur la folioj kaj fajrerante
        defaladis<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la arboj &jcirc;etadis <tld/>antajn ombrojn sur la
        herbon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind><tld/>iganta</ind>j impresoj<fnt>B</fnt>.
        <trd lng="hu">megrend&iacute;t&odblac;</trd>
        <trd lng="ru">&c_p;&c_r;&c_i;&c_v;&c_o;&c_d;&c_ja;&c_shch;&c_i;&c_j; &c_v;
<ind>&c_t;&c_r;&c_je;&c_p;&c_je;&c_t;</ind></trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_r;&c_y;&c_zh;&c_e;&c_c;&c_mol;</trd>,
    <trd>&c_t;&c_r;&c_y;&c_m;&c_c;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_t;&c_r;&c_y;&c_m;&c_c;&c_je;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">tremolar</trd>
  <trdgrp lng="de">
    <trd>zittern</trd>,
    <trd>beben</trd>
  </trdgrp>
  <trd lng="en">tremble</trd>
  <trd lng="es">temblar</trd>
  <trd lng="fr">trembler</trd>
  <trdgrp lng="hu">
    <trd>remeg</trd>,
    <trd>reszket</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>beven</trd>,
    <trd>trillen</trd>,
    <trd>bibberen</trd>
  </trdgrp>
  <trd lng="pt">tremer</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_r;&c_o;&c_zh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_o;&c_d;&c_r;&c_o;&c_g;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_p;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="trem.0o">
  <kap><tld/>o, <var><kap><tld/>ado</kap></var></kap>
  <snc mrk="trem.0o.MOV">
    <dif>
      Movi&gcirc;o de io <tld/>anta:
      <ekz>
        <tld/>ado trakuris &scirc;ian korpon;
      </ekz>
      <ekz>
        ter<tld/>o detruis preska&ubreve; &ccirc;iujn domojn de la
        urbeto.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vibr.0o.MOV">vibro</ref>
    </refgrp>
  </snc>
  <trd lng="fr">temblement</trd>
  <trd lng="nl">bibberen<klr tip="ind"> (zn.)</klr></trd>
  <trd lng="ru">&c_d;&c_r;&c_o;&c_zh;&c_mol;</trd>
</drv>

<drv mrk="trem.ek0i">
  <kap>ek<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="trem.0i.komenci">
    <dif>
      Komenci <tld/>i:
      <ekz>
        la turmentata animo trarigardis spirite &ccirc;iujn agojn
        de sia tera vivo kaj ek<tld/>is sub larmoj tiel, 
        kiel Inger neniam estis havinta la kapablon plori
        <fnt>
          <bib>Fab4</bib>,
          <lok>Knabino, kiu pa&scirc;is sur panon</lok>
        </fnt>;
      </ekz>
      <ekz>
        atakis teruro, ek<tld/>is la kruro
	<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la senkolori&gcirc;intaj pupiloj de la maljunulino ne
        malseki&gcirc;is per larmo kaj ne eklumis e&ccirc; per la plej
        malgranda brilo, kiam &scirc;i ekparolis la lastajn vortojn,
        sed &scirc;ia pala mallar&gcirc;a bu&scirc;o ek<tld/>is inter
        la amaso de la &gcirc;in &ccirc;irka&ubreve;antaj sulketoj kaj la kava
        brusto ek<tld/>is sub la antikvamoda manteleto
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="fr">frissonner</trd>
    <trd lng="hu">elkezd <ind>remegni</ind></trd>
    <trdgrp lng="nl">
      <trd>beginnen te <ind>beven</ind></trd>,
      <trd>beginnen te <ind>trillen</ind></trd>,
      <trd>beginnen te <ind>bibberen</ind></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_d;&c_r;&c_o;&c_zh;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_t;&c_r;&c_je;&c_p;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="trem.0i.eri">
    <ref tip="dif" cel="trem.0eri"><tld/>eri</ref>
    <trdgrp lng="hu">
      <trd>megremeg</trd>,
      <trd>&ouml;sszerezzen</trd>
    </trdgrp>
    <trd lng="nl">ineen<ind>krimpen</ind><klr tip="ind">
    (v.d.schrik)</klr></trd>
  </snc>
</drv>

<drv mrk="trem.0eri">
  <kap><tld/>eri</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="trem.0eri.MOV">
    <dif>
      Fari unuopan <tld/>movon:
      <ekz>
        parolante, la gasto plurfoje angore <tld/>eris
        <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 13a</lok></fnt>;
      </ekz>
      <ekz>
        sed se &scirc;i estis ekparolanta, do tiel subite kaj la&ubreve;te,
        ke oni <tld/>eris
        <fnt>
          <aut>I. Bunin, trad. G. Arosev</aut>, <vrk>Rusja</vrk>,
          <lok>en: Rusaj amnoveloj, 2000.</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">zusammenzucken</trd>
  <trd lng="en">to <ind>start</ind></trd>
  <trdgrp lng="hu">
    <trd>megremeg</trd>,
    <trd>&ouml;sszerezzen</trd>
  </trdgrp>
  <trd lng="fr">sursauter</trd>
   <trd lng="nl">ineen<ind>krimpen</ind><klr tip="ind">
   (v.d.schrik)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_r;&c_o;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_z;&c_d;&c_r;&c_o;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

</art>
