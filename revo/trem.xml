<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>trem</rad>/i
</kap>
<drv mrk="trem.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti movata tien kaj reen per malgrandaj sinsekvaj skuetoj:
      <ekz>
        la ponto <tld/>as kiam oni veturas sur &gcirc;i<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i pro frosto, pro timo;
      </ekz>
      <ekz>
        lia vo&ccirc;o, liaj lipoj <tld/>is<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i kiel a&ubreve;tuna folio<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind>ek<tld/>i</ind>s kaj ekskui&gcirc;is la tero<fnt>Z</fnt>;
        <trd lng="hu">megremeg</trd>
        <trd lng="ru">&c_z;&c_a;&c_d;&c_r;&c_o;&c_zh;&c_a;&c_t;&c_mol;</trd>
      </ekz>
      <ekz>
        pluvaj gutoj <tld/>is sur la folioj kaj fajrerante
        defaladis<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la arboj &jcirc;etadis <tld/>antajn ombrojn sur la herbon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind><tld/>ado</ind> trakuris &scirc;ian korpon;
        <trd lng="hu">remeg&eacute;s</trd>
        <trd lng="ru">&c_d;&c_r;&c_o;&c_zh;&c_mol;</trd>
      </ekz>
      <ekz>
        <ind><tld/>iganta</ind>j impresoj<fnt>B</fnt>;
        <trd lng="hu">megrend&iacute;t&odblac;</trd>
        <trd lng="ru">&c_p;&c_r;&c_i;&c_v;&c_o;&c_d;&c_ja;&c_shch;&c_i;&c_j; &c_v; <ind>&c_t;&c_r;&c_je;&c_p;&c_je;&c_t;</ind></trd>
      </ekz>
      <ekz>
        <ind>ter<tld/>o</ind> detruis preska&ubreve; &ccirc;iujn domojn de la urbeto.
        <trd lng="be">&c_z;&c_je;&c_m;&c_l;&c_ja;&c_t;&c_r;&c_u;&c_s;</trd>
        <trd lng="hu">f&ouml;ldreng&eacute;s</trd>
        <trd lng="ru">&c_z;&c_je;&c_m;&c_l;&c_je;&c_t;&c_r;&c_ja;&c_s;&c_je;&c_n;&c_i;&c_je;</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_r;&c_y;&c_zh;&c_e;&c_c;&c_mol;</trd>,
    <trd>&c_t;&c_r;&c_y;&c_m;&c_c;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_t;&c_r;&c_y;&c_m;&c_c;&c_je;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>zittern</trd>,
    <trd>beben</trd>
  </trdgrp>
  <trd lng="en">tremble</trd>
  <trdgrp lng="hu">
    <trd>remeg</trd>,
    <trd>reszket</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>beven</trd>,
    <trd>trillen</trd>,
    <trd>bibberen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_r;&c_o;&c_zh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_o;&c_d;&c_r;&c_o;&c_g;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_p;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="trem.0brili">
  <kap><tld/>brili</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Intermite &jcirc;eti lumbrilojn:
      <ekz>
        la steloj <tld/>brilis en la silenta nokto.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ib;&c_g;&c_c;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_ib;&c_g;&c_a;&c_c;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>flimmern</trd>,
    <trd>glitzern</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>villog</trd>,
    <trd>csillog</trd>
    <trd>vill&oacute;dzik</trd>
  </trdgrp>
  <trd lng="nl">flikkeren</trd>
  <trd lng="ru">&c_m;&c_je;&c_r;&c_c;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
