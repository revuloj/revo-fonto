<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: norvegi.xml,v 1.16 2021/04/12 07:08:00 revo Exp $">
<kap>
  <rad>Norvegi</rad>/o
</kap>

<drv mrk="norvegi.N0o">
  <kap><tld/>o</kap>
  <snc mrk="norvegi.N0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="norveg.N0ujo.GEOG">Norvegujo</ref>:
      <ekz>
        <tld lit="N"/>o estas lando en Norda E&ubreve;ropo, sur la
        Skandinava duoninsulo.
        <fnt><bib>Viki</bib>,
          <lok><url
          ref="http://eo.wikipedia.org/wiki/Norvegio">Norvegio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;efkomitato en <tld/>o provas starigi komitatojn en aliaj landoj por varbi
        virinojn por partopreni en la mar&scirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Douglas Draper</aut>
          <vrk><url ref="https://www.monato.be/2003/008001.html"
              >Pa&scirc;oj al paco</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        la armeoj sovetia, usona, <tld lit="n"/>a
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006947.html"
              >Reago: Eks-Jugoslavio: kiu kiun mistifikas?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_N;&c_a;&c_r;&c_v;&c_e;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_N;&c_o;&c_r;&c_v;&c_je;&c_g;&c_i;&c_ja;</trd>
  <trd lng="cs">Norsko</trd>
  <trd lng="de">Norwegen</trd>
  <trd lng="en">Norway</trd>
  <trd lng="es">Noruega</trd>
  <trd lng="fa">&a_n;&a_r;&a_w;&f_zh;</trd>
  <trdgrp lng="fr">
    <trd>Norv&egrave;ge</trd>,
    <trd>Royaume de Norv&egrave;ge</trd>
  </trdgrp>
  <trd lng="id">Norwegia</trd>
  <trd lng="nl">Noorwegen</trd>
  <trd lng="no">Norge</trd>
  <trdgrp lng="pl">
    <trd>Norwegia</trd>,
    <trd>Kr&oacute;lestwo <ind>Norwegii</ind></trd>
  </trdgrp>
  <trd lng="pt">Noruega</trd>
  <trd lng="sk">N&oacute;rsko</trd>
</drv>
</art>

