      <ref tip="sin" cel="okaz.0e.situacie">okaze</ref>,
      <ref tip="sin" cel="temp.de0oal0o">de tempo al tempo</ref>:
      <ekz>
        kelk<tld/> reaperis la eta rideto
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
