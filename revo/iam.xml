    <trd>ehemaliger</trd>,
    <trd>gewesener</trd>
  </trdgrp>
  <trd lng="en">one-time</trd>
  <trdgrp lng="es">
    <trd>antiguo<klr tip="ind"> (del pasado)</klr></trd>,
    <trd>del <ind>pasado</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
