<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: porcx.xml,v 1.5 2023/03/18 09:23:47 revo Exp $">
<kap>
  <rad>por&ccirc;</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="porcx.0o">
  <kap><tld/>o</kap>
  <snc mrk="porcx.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Anta&ubreve;konstrua&jcirc;o kiu &scirc;irmas spacon anta&ubreve; <ref cel="pord.0ego">pordego</ref>:
      <ekz>
        <nom>Lizaveta Ivanovna</nom> estis forportita al la pre&gcirc;eja 
        <tld/>o
        <fnt>
          <aut>A. Pu&scirc;kin, trad. A. Kor&jcirc;enkov</aut>,
          <vrk><url ref="http://www.esperanto-ondo.ru/Libroj/V_bibl01.pdf">
          La pika damo</url></vrk>,
          <lok>2003-2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aler.0o">alero</ref>,
      <ref cel="markez.0o.ARKI">markezo</ref>,
      <ref cel="portik.0o">portiko</ref>,
      <ref cel="verand.0o">verando</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Vorbau</trd>,
    <trd>Vordach</trd>
  </trdgrp>
  <trd lng="en">porch</trd>
  <trd lng="fr">porche</trd>
  <trd lng="pl">prothyron</trd>
  <trd lng="ro">prothyron</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_r;&c_t;&c_i;&c_k;</trd>,
    <trd>&c_p;&c_ib;&c_d;&#8217;&c_ji;&c_z;&c_d;</trd>,
    <trd>&c_k;&c_r;&c_i;&c_t;&c_i;&c_j; &c_v;&c_h;&c_ib;&c_d;</trd>,
    <trd>&c_s;&c_ib;&c_n;&c_i;</trd>,
    <trd>&c_d;&c_a;&c_sh;&c_o;&c_k; &c_n;&c_a;&c_d; &c_v;&c_h;&c_ib;&c_d;&c_n;&c_i;&c_m;&c_i; &c_d;&c_v;&c_je;&c_r;&c_i;&c_m;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_p;&c_je;&c_r;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>

