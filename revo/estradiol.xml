<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: estradiol.xml,v 1.4 2023/06/21 15:27:21 revo Exp $">
<kap>
    <rad>estradiol</rad>/o
</kap>
<drv mrk="estradiol.0o">
  <kap><tld/>o</kap>
  <snc mrk="estradiol.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      <ref tip="super" cel="estrogen.0o.FIZL">Estrogeno</ref>, kiu estas
      precipe produktita en la ovariaj folikloj:
      <ekz>
        naturaj estrogenoj estas la foliklohormonoj <tld/>o, estrono
        kaj estriolo
        <fnt>
          <vrk>Medicinalaj Plantoj kaj Sufero de la Prostato</vrk>,
          <lok><url
          ref="https://scienca-revuo.info/article/download/115/94">Scienca
          Revuo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        ekzemploj de steroidaj hormonoj inkludas la seksajn hormonojn
        <tld/>o kaj testosterono
        <fnt>
          <vrk>Hormono</vrk>
          <lok><url
          ref="&Viki;/Hormono">Vikipedio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_s;&c_t;&c_r;&c_a;&c_d;&c_y;&c_jo;&c_l;</trd>
  <trd lng="ca">estradiol</trd>
  <trd lng="cs">estradiol</trd>
  <trd lng="de">&Ouml;stradiol</trd>
  <trdgrp lng="en">
    <trd>estradiol</trd>,
    <trd>oestradiol</trd>
  </trdgrp>
  <trd lng="es">estradiol</trd>
  <trd lng="fr">&oelig;stradiol</trd>
  <trd lng="gl">estradiol</trd>
  <trd lng="it">estradiolo</trd>
  <trdgrp lng="nl">
    <trd>oestradiol</trd>,
    <trd>estradiol</trd>
  </trdgrp>
  <trd lng="oc">estradi&ograve;l</trd>
  <trd lng="ru">&c_e;&c_s;&c_t;&c_r;&c_a;&c_d;&c_i;&c_o;&c_l;</trd>
  <trd lng="sv">&ouml;stradiol</trd>
</drv>
</art>

