<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>*</ofc><ofc>7</ofc><rad>Sofi</rad>/o
</kap>
<drv mrk="sofi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">NOM</uzo>
    <dif>
      Virina <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> grekdevena:
      <ekz>
        Johanon, (...) kaj Sofion iliaj gepatroj nomas Johan&ccirc;jo (a&ubreve;
        Jo&ccirc;jo), (...) kaj Sonjo (a&ubreve; Sofinjo).
        <fnt><bib>F</bib>, <lok>&FE; 38</lok></fnt>.
      </ekz>
    </dif>
  </snc>     
  <trdgrp lng="en">
    <trd>Sofia</trd>
    <trd>Sophia</trd>
    <trd>Sophie</trd>,
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Sofia</trd>
    <trd>Sophia</trd>
    <trd>Sophie</trd>,
  </trdgrp>
</drv>

<drv mrk="sofi.0njo">
  <kap><ofc>*</ofc><ofc>ekz</ofc><tld/>njo,
  <var><kap>Sonjo</kap></var></kap>
  <snc>
    <dif>
      Karesa diminutivo de Sofio:
      <ekz>
        Johanon, (...) kaj Sofion iliaj gepatroj nomas Johan&ccirc;jo (a&ubreve;
        Jo&ccirc;jo), (...) kaj Sonjo (a&ubreve; Sofinjo).
        <fnt><bib>F</bib>, <lok>&FE; 38</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">Soph'</trd>
</drv>
</art>

<!--
$Log$
