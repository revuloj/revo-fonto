<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: merop.xml,v 1.9 2023/09/02 06:04:11 revo Exp $">
<kap>
  <rad>merop</rad>/o
  <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="merop.0o">
  <kap><tld/>o</kap>
  <snc mrk="merop.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="mangx.abel0ulo.ZOO">Abelman&gcirc;ulo</ref>:
      <ekz>
        <tld/>oj, supre rozaj kaj sube hel-bluaj, flugis en la sunbrilo, 
        kaptante en la aero abelojn kaj cikadojn
        <fnt>
          <aut>H. Sienkiewicz, trad. M. Sygnarski</aut>,
          <vrk><url ref="http://esperanto.pl/page.php?tid=200000">
          Tra dezerto kaj praarbaro</url></vrk>,
          <lok>Varsovio, 1978</lok>
        </fnt>;
      </ekz>
      <ekz>
        balda&ubreve; ne nur lia propra domo sed anka&ubreve; la ceteraj
        plenis de <tld/>oj, kanarioj, kalandroj
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de
        Diego</aut>, <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="ca">abellerol</trd>
    <trd lng="de">Bienenfresser</trd>
    <trd lng="en">bee-eater</trd>
    <trd lng="fr">gu&ecirc;pier<klr> (oiseau)</klr></trd>
    <trd lng="hu">gyurgyalag</trd>
    <trd lng="nl">bijeneter</trd>
    <trd lng="la">Merops apiaster</trd>
    <trd lng="pl">&zdot;o&lstroke;na<klr> (nie:
    pszczo&lstroke;ojad!)</klr></trd>
    <trd lng="pt">abelaruco<klr> (comum)</klr></trd>
    <trd lng="ru">&c_o;&c_s;&c_o;&c_je;&c_d;</trd>
    <trd lng="sv">bi&auml;tare</trd>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_a;&c_je;&c_d;</trd>
  <trd lng="cs">vlha</trd>
  <trd lng="ja">&#x30CF;&#x30C1;&#x30AF;&#x30A4;</trd>
  <trd lng="sk">vlha</trd>
  <trd lng="uk">&#x431;&#x434;&#x436;&#x43E;&#x43B;&#x43E;&#x457;&#x434;&#x43A;&#x430;</trd>
</drv>
</art>

