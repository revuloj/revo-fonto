<vortaro>
<art mrk="$Id$">
<kap><rad>kastored</rad>/oj</kap>
<drv mrk="kastor1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="rod1.0uloj">roduloj</ref>
      (<trd lng="la">Castoridae</trd>), kun la genro
      <ref tip="sub" cel="kastor.0o">kastoro</ref>.
    </dif>
  </snc>
  <trd lng="de">Biber</trd>
  <trd lng="en">beavers</trd>
  <trd lng="nl">bevers</trd>
  <trd lng="ru">&c_b;&c_o;&c_b;&c_r;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>

