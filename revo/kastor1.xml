<art mrk="$Id: kastor1.xml,v 1.16 2023/12/03 08:00:24 revo Exp $">
<kap>
   <rad>kastored</rad>/oj
</kap>
<drv mrk="kastor1.0oj">
   <kap><tld/>oj</kap>
   <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
       <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo 
       <ref tip="malprt" cel="rongx.0uloj">ron&gcirc;uloj</ref>
       (<trd lng="la">Castoridae</trd>), kun la genro
       <ref tip="sub" cel="kastor.0o">kastoro</ref>:
      <ekz>
        la kastoro el la <klr>[&#x2026;]</klr> familio de <tld/>oj troveblas en la norda parto
        de E&ubreve;razio kaj Nordameriko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17753">Kastoro</url></lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_b;&c_a;&c_b;&c_r;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">cast&ograve;rids</trd>
  <trd lng="cs">bobrovit&iacute;</trd>
  <trd lng="de">Biber</trd>
  <trd lng="en">beavers</trd>
  <trd lng="fr">castorid&eacute;s</trd>
  <trd lng="hu">h&oacute;df&eacute;l&eacute;k</trd>
  <trd lng="nl">bevers</trd>
  <trd lng="pl">bobrowate</trd>
  <trd lng="ru">&c_b;&c_o;&c_b;&c_r;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">bobrovit&eacute;</trd>
  <trd lng="zh">&#x6CB3;&#x72F8; <pr>h&#xE9;l&#xED;</pr></trd>
</drv>
</art>
