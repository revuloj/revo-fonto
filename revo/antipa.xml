<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: antipa.xml,v 1.26 2025/08/30 06:38:51 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>antipati</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="antipa.0o">
  <kap><tld/>o</kap>
  <snc mrk="antipa.0o.malsimpatio">
    <dif>
      <ref tip="dif" cel="simpat.mal0o">Malsimpatio</ref>:
      <ekz>
        &ccirc;ar al multaj homoj, kiuj ne kutimis donadi al si precizan kalkulon pri siaj
        simpatioj kaj <tld/>oj, ordinare &scirc;ajnas, ke se ili ne aprobas tiun a&ubreve; alian
        formon de ia ideo, ili nepre devas ataki la ideon mem entute
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        stratbataloj vekis <tld/>on
        <fnt>
        <aut>Ji&rcaron;&iacute; Patera</aut>,
        <vrk>Post la kunsido de financistoj en Prago</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2001a, numero 1a, p. 12a</lok>
        </fnt>;
      </ekz>
      <ekz>
        malgra&ubreve; plii&gcirc;anta <tld/>o kontra&ubreve; e&ubreve;ropaj judoj, E&ubreve;ropo mem ne estas antisemita
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2004/008331.html">Judaj premgrupoj en Bruselo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="inklin.mal0o">malinklino</ref>
    <trdgrp lng="zh">
      <trd>&#21388;&#24694; <pr>y&agrave;nw&ugrave;</pr></trd>,
      <trd>&#21421;&#24801; <pr>y&agrave;nw&ugrave;</pr></trd>,
      <trd>&#21453;&#24863; <pr>f&#462;ng&#462;n</pr></trd>
    </trdgrp>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>malsimpatio</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_a;&c_n;&c_t;&c_y;&c_p;&c_a;&c_t;&c_y;&c_ja;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_r;&c_y;&c_ja;&c_z;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>antipatie</trd>,
    <trd>nechu&tcaron;</trd>,
    <trd>odpor</trd>,
    <trd>odpudivost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Antipathie</trd>,
    <trd>Abneigung</trd>
  </trdgrp>
  <trd lng="en">antipathy</trd>
  <trd lng="es">antipat&iacute;a</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&f_y;&a_z;&a_A;&a_r;&f_y;</trd>,
    <trd>&a_t;&a_n;&a_f;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">antipathie</trd>
  <trd lng="he">&samekh;&#1456;&lamed;&#1460;&yod;&dalet;&#1464;&he;</trd>
  <trd lng="id">antipati</trd>
  <trdgrp lng="ja">
    <trd>&#21453;&#24863;  <pr>&#12399;&#12435;&#12363;&#12435;</pr></trd>,
    <trd>&#27611;&#23244;&#12356;  <pr>&#12369;&#12366;&#12425;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_t;&c_i;&c_p;&c_a;&c_t;&c_i;&c_ja;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_r;&c_i;&c_ja;&c_z;&c_n;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>antipatia</trd>,
    <trd>nechu&tcaron;</trd>,
    <trd>odpor</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&c_n;&c_t;&c_i;&c_p;&c_a;&c_t;&c_ib;&c_ja;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_r;&c_i;&c_ja;&c_z;&c_n;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="antipa.0a">
  <kap><tld/>a</kap>
  <snc mrk="antipa.0a.kauxzanta">
    <dif>
      Ka&ubreve;zanta <tld/>on:
      <ekz>
        <tld/>a homo;
      </ekz>
      <ekz>
        <tld/>a estis al mi tiu kutimo;
      </ekz>
      <ekz>
        <tld/>a estis al <nom>Joab</nom> la vorto de la re&gcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>I. Kroniko 21:6</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#23521;&#24681; <pr>gu&#462;&emacron;n</pr></trd>
</snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_n;&c_t;&c_y;&c_p;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_r;&c_y;&c_je;&c_m;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>odporn&yacute;</trd>,
    <trd>protivn&yacute;</trd>
  </trdgrp>
  <trd lng="de">unsympathisch</trd>
  <trd lng="en">antipathetic</trd>
  <trdgrp lng="fa">
    <trd>&a_n;&a_A;&a_kh;&a_w;&a_sh;&a_A;&f_y;&a_n;&a_d;</trd>,
    <trd>&a_z;&a_n;&a_n;&a_d;&a_h;</trd>,
    <trd>&a_t;&a_n;&a_f;&a_r;&a_A_madda;&a_w;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">antipathique</trd>
  <trd lng="he">&samekh;&#1456;&lamed;&#1460;&yod;&dalet;&#1464;&he;</trd>
  <trd lng="id">antipatik</trd>
  <trdgrp lng="ja">
    <trd>&#21453;&#24863;&#12434;&#35226;&#12360;&#12373;&#12379;&#12427;
    <pr>&#12399;&#12435;&#12363;&#12435;&#12434;&#12362;&#12412;&#12360;&#12373;&#12379;&#12427;</pr></trd>,
    <trd>&#34411;&#12398;&#22909;&#12363;&#12394;&#12356;  <pr>&#12385;&#12421;&#12358;&#12398;&#12377;&#12363;&#12394;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_t;&c_i;&c_p;&c_a;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>antipatick&yacute;</trd>,
    <trd>nepr&iacute;jemn&yacute;</trd>,
    <trd>protivn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&c_n;&c_t;&c_i;&c_p;&c_a;&c_t;&c_i;&c_ch;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_r;&c_i;&c_jeu;&c_m;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="antipa.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="antipa.0i.malsimpatii">
    <dif>
      <ref tip="dif" cel="simpat.mal0i">Malsimpatii</ref>:
      <ekz>
        <tld/>i iun, al iu;
      </ekz>
      <ekz>
        ili <tld/>is la ideon pri la angla kiel &leftquot;la&rightquot; internacia lingvo
        <fnt>
          <vrk><url ref="http://www.espero.com.cn/2014-01/06/content_31101253.htm">Partopreninte la
          32-an Komunan Seminarion de Aziaj Junularoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is &scirc;in post ilia unua renkonto kaj &ccirc;iel evitis la novajn, du a&ubreve; tri
        fojojn li e&ccirc; estis mal&gcirc;entila dum iliaj interparoladoj
        <fnt>
          <vrk><url ref="http://www.esperanto.mv.ru/Ojstro/Traduko/p2c3.html">Ojstro verkas
          pamfletojn</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxat.mal0i">mal&scirc;ati</ref>,
      <ref cel="am.mal0i">malami</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_c;&c_mol; <ind>&c_a;&c_n;&c_t;&c_y;&c_p;&c_a;&c_t;&c_y;&c_ju;</ind></trd>,
    <trd>&c_a;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_c;&c_mol; <ind>&c_n;&c_je;&c_p;&c_r;&c_y;&c_ja;&c_z;&c_mol;</ind></trd>
  </trdgrp>
  <trd lng="cs">m&iacute;t k n&ecaron;komu antipatii</trd>
  <trdgrp lng="de">
    <trd>Abneigung haben</trd>,
    <trd>unangenehm <ind>finden</ind></trd>,
    <trd>unsympathisch <ind>finden</ind></trd>
  </trdgrp>
  <trd lng="en">antipathize</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&f_y;&a_z;&a_A;&a_r; &a_b;&a_w;&a_d;&a_n;</trd>,
    <trd>&a_t;&a_n;&a_f;&a_r; &a_d;&a_A;&a_sh;&a_t;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">ressentir de l'<ind>antipathie</ind> (pour)</trd>
  <trd lng="he">&lamed;&alef; &alef;&vav;&he;&bet;</trd>
  <trd lng="id"><baz>antipati</baz>berantipati</trd>
  <trdgrp lng="ja">
    <trd>&#21453;&#24863;&#12434;&#12418;&#12388;  <pr>&#12399;&#12435;&#12363;&#12435;&#12434;&#12418;&#12388;</pr></trd>,
    <trd>&#27611;&#23244;&#12356;&#12377;&#12427;  <pr>&#12369;&#12366;&#12425;&#12356;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;
    &c_a;&c_n;&c_t;&c_i;&c_p;&c_a;&c_t;&c_i;&c_ju;</trd>,
    <trd>&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;
    &c_n;&c_je;&c_p;&c_r;&c_i;&c_ja;&c_z;&c_n;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ma&tcaron; antipatiu <klr>(k niekomu, nie&ccaron;omu)</klr></trd>,
    <trd>nezn&aacute;&scaron;a&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#23244;&#24323; <pr>xi&aacute;nq&igrave;</pr></trd>,
    <trd>&#23244;&#26820; <pr>xi&aacute;nq&igrave;</pr></trd>
  </trdgrp>
</drv>
</art>
