  <rad>lev</rad>/i
</kap>

<drv mrk="lev.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="lev.0i.altigi">
    <dif>
      Movi de malsupre supren; 
      pli<ref tip="sin" cel="alt.0igi">altigi</ref>:
      <ekz>
        <tld/>i pezan &scirc;ar&gcirc;on, kru&ccirc;on, la bastonon 
        kontra&ubreve; iu;
      </ekz><ekz>
        <tld/>i flagon &gcirc;is la supro de la masto;
      </ekz><ekz>
        <tld/>i la rulkurtenon de fenestro;
      </ekz><ekz>
        la Eternulo volis <tld/>i <nom>Elijan</nom> en ventego en
        &ccirc;ielon
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 2:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vento <tld/>as la ondojn, polvon;
      </ekz><ekz>
        <tld/>i la brakon, piedon, kapon; 
      </ekz>
      <ekz>
         <tld/>i la palpebrojn
         <fnt><bib>Marta</bib></fnt>;
      </ekz><ekz>
        &scirc;ultrojn, la okulojn de sur la pa&gcirc;o;
      </ekz>
      <ekz>
        li <tld/>is la mantelon de <nom>Elija</nom>, kiu defalis de li
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 2:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        la profeto <tld/>is la kadavron <klr>[&#x2026;]</klr> kaj metis &gcirc;in sur
        la azenon
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 13:29</lok>
        </fnt>;
      </ekz><ekz>
        sin <tld/>i sur la &scirc;tuparo, en la aeron;
      </ekz><ekz>
        orkovritaj kupoloj sin <tld/>adis super la
        tegmentoj
         <fnt><bib>Fab1</bib>, <lok>La virineto de maro</lok></fnt>;
      </ekz><ekz>
        la suno, tago, luno sin <tld/>as;
      </ekz><ekz>
        el<tld/>i akvon el la puto.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>heben</trd>,
      <trd>anheben</trd>,
      <trd>erheben</trd>,
      <trd>lupfen</trd>
    </trdgrp>
    <trd lng="es">elevar</trd>
    <trd lng="fr">lever</trd>
    <trdgrp lng="hu">
      <trd>emel</trd>,
      <trd>felemel</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>opheffen</trd>,
      <trd>optillen</trd>
    </trdgrp>
    <trd lng="pl">podnosi&cacute;</trd>
  </snc>
  <snc mrk="lev.0i.starigi">
    <dif>
      Meti stare tion, kio estis ku&scirc;anta, klinita ktp:
      <ekz>
        <tld/>i malsanulon de lia se&gcirc;o, falintan infanon;
      </ekz><ekz>
        <tld/>i standardon
         <fnt><bib>MT</bib>, <lok>Psalmaro 60:4</lok></fnt>;
      </ekz><ekz>
        sin <tld/>i de, el sia lito (elliti&gcirc;i);
      </ekz><ekz>
        maro <tld/>as siajn ondojn
        <fnt><bib>MT</bib>, <lok>Je&hcirc;ezkel 26:2?</lok></fnt>;
      </ekz>
      <ekz>
        Mi <tld/>os <tld/>i <klr>(ribeligos)</klr> kontra&ubreve; vin multe da
        nacioj
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 26:3</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>aufrichten</trd>,
      <trd>anheben</trd>,
      <trd>erheben</trd>,
      <trd>lupfen</trd>
    </trdgrp>
    <trd lng="es">levantar</trd>
    <trdgrp lng="fr">
      <trd>mettre <ind>debout</ind></trd>,
      <trd>relever<klr tip="ind"> (mettre debout)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>felemel</trd>,
      <trd>fel&aacute;ll&iacute;t</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>recht zetten</trd>,
      <trd>oprichten</trd>
    </trdgrp>
    <trd lng="pl">postawi&cacute;</trd>
  </snc>
  <snc mrk="lev.0i.komenci">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Iniciati:
      <ekz>
        se la demando estus <tld/>ita anta&ubreve; <klr>[&#x2026;]</klr>
        <fnt>
          <bib>LR</bib>
          <lok>4. Fonetiko</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj politikistoj <tld/>is tiun &ccirc;i demandon
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2004/008335.html">Konstitucio &ccirc;u la civitanoj
          decidu?</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        malgra&ubreve; la ironia tono de la &gcirc;isnunaj vortoj, mi agnoskas, ke la
        afero vo&ccirc;e
        <tld/>ita neniel bagatelas
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2012/010885.php">Vo&ccirc;o de ripro&ccirc;o</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>stellen</trd>
    </trdgrp>
    <trd lng="fr">soulever<klr tip="ind"> (question
    etc.)</klr></trd>
    <trdgrp lng="hu">
      <trd>felvet</trd>,
      <trd>kezdem&eacute;nyez</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>stellen<klr tip="ind"> (een vraag)</klr></trd>,
      <trd>voorstellen</trd>
    </trdgrp>
  </snc>
  <snc mrk="lev.0i.pliigi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Pliigi, plialtigi la gradon de io:
      <ekz>
        <tld/>i <klr>(plila&ubreve;tigi)</klr> sian vo&ccirc;on
         <fnt><bib>NT</bib>, <lok>La agoj 2:14</lok></fnt>;
      </ekz>
      <ekz>
        politikistoj, kiuj farus e&ccirc; ion pli teruran por <tld/>i sian
        popularecon
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2005/008601.php">Torturado televida</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>heben</trd>,
      <trd>anheben</trd>,
      <trd>erh&ouml;hen</trd>,
      <trd>steigern</trd>
    </trdgrp>
    <trd lng="es">elevar</trd>
    <trd lng="fr">&eacute;lever<klr tip="ind"> (renforcer)</klr></trd>
    <trdgrp lng="hu">
      <trd>n&ouml;vel</trd>,
      <trd>er&odblac;s&iacute;t</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>verheffen</trd>,
      <trd>verhogen</trd>
    </trdgrp>
  </snc>
  <snc mrk="lev.0i.nobligi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Meti en pli altan staton; spirite, morale plialtigi:
      <ekz>
        ni celas <tld/>i la moralan kaj fartan staton de la popolo;
      </ekz><ekz>
        tio, kio propre <tld/>is la homaron alte super &ccirc;iuj
        aliaj bestoj
         <fnt><bib>EE</bib>, <lok>&ccirc;ap. 2</lok></fnt>;
      </ekz><ekz>
        <tld/>i la benketon de botisto al la indo de instrua
        se&gcirc;o de filozofo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz><ekz>
        <tld/>i ion &gcirc;is principo;
      </ekz><ekz>
        sama sento (alten) <tld/>is &ccirc;iujn korojn;
      </ekz><ekz>
        plej timu la homo, se dioj lin <tld/>is
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>!
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>heben</trd>,
      <trd>steigern</trd>,
      <trd>erheben</trd>
    </trdgrp>
    <trd lng="fr">&eacute;lever<klr tip="ind"> (moralement)</klr></trd>
    <trd lng="hu">felemel <klr tip="ind">(mor&aacute;lisan)</klr></trd>
    <trdgrp lng="nl">
      <trd>verheffen</trd>,
      <trd>verhogen</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_d;&c_n;&c_ja;&c_c;&c_mol;</trd>,
      <trd>&c_u;&c_z;&c_mol;&c_n;&c_ja;&c_c;&c_mol;</trd>,
      <trd>&c_u;&c_z;&c_v;&c_y;&c_s;&c_ib;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_d;&c_v;&c_y;&c_s;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&#xF56;&#xF40;&#xFB1;&#xF42;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>vyzvednout</trd>,
    <trd>zvednout</trd>
  </trdgrp>
  <trd lng="en">lift</trd>
  <trd lng="id">meng<ind>angkat</ind></trd>
  <trd lng="pl">podnosi&cacute;</trd>
  <trdgrp lng="pt">
    <trd>erguer</trd>,
    <trd>suspender</trd>,
    <trd>elevar</trd>,    
    <trd>levantar</trd>,
    <trd>guindar</trd>,
    <trd>altear</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_n;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dv&iacute;ha&tcaron;</trd>,
    <trd>zdvihn&uacute;&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x456;&#x434;&#x43D;&#x456;&#x43C;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x456;&#x434;&#x456;&#x439;&#x43C;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x437;&#x434;&#x456;&#x439;&#x43C;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x437;&#x43D;&#x43E;&#x441;&#x438;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x456;&#x434;&#x432;&#x438;&#x449;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="lev.0o">
  <kap><tld/>o</kap>
  <snc mrk="lev.0o.ago">
    <dif>
      Ago <tld/>i, rezulto de tiu ago:
      <ekz>
        per <tld/>o de la mano Vi promesis doni <klr>[la
        landon]</klr> 
        al ili
        <fnt><bib>MT</bib><lok>&Neh; 9:15</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>o de la apostoloj al sacerdota rango
        <fnt>
          <bib>Monato</bib>
          <aut>Pastro Duilio Magnani</aut>
          <vrk><url ref="&Steloj;monato2/007299.html">Fondi eklezion?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de vo&ccirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url ref="&Monato;/2004/008372.html">Dujarcenta ideo</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        knabino <klr>[&#x2026;]</klr> flirtas per <tld/>o de sia granda,
        brodita &scirc;alo
        <fnt>
          <bib>Monato</bib>
          <aut>Husejn Al-Amily</aut>
          <vrk><url ref="&Monato;/2007/009296.php">La vualo, romantika kaj
          erotika</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz><ekz>
        vi, kun subita, spasma pugo<tld/>o ekrigidi&gcirc;as en ekstaza 
        pont'
        <fnt><aut>P. Peneter</aut><vrk>Sekretaj 
            Sonetoj</vrk><lok>XXXVII</lok></fnt>;
      </ekz><ekz>
        kun kolera <ind>&scirc;ultro<tld/>o</ind> li for&jcirc;etis la ideon
        <fnt><bib>UrdHadda</bib><lok>p. 10</lok></fnt>.
        <trd lng="fr"> haussement d'&eacute;paules</trd>
        <trd lng="hu">v&aacute;llr&aacute;nd&iacute;t&aacute;s</trd>
      </ekz>
    </dif>
  </snc>
  <refgrp tip="hom">
    Ne konfuzu kun
    <ref cel="lev1.0o">levo</ref> (bulgara monunuo).
  </refgrp>
  <trdgrp lng="de">
    <trd>Heben</trd>,
    <trd>Anheben</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>lever<klr tip="ind"> (fait de lever)</klr></trd>,
    <trd>&eacute;l&eacute;vation<klr tip="ind"> (fait de lever)</klr></trd>,
    <trd>haussement<klr tip="ind"> (fait de lever)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>emel&eacute;s</trd>,
    <trd>felemel&eacute;s</trd>,
    <trd>fel&aacute;ll&iacute;t&aacute;s</trd>,
    <trd>felvet&eacute;s</trd>
  </trdgrp>
  <trd lng="pl">podniesienie</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_malmol;&c_jo;&c_m;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_n;&c_ja;&c_t;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>zdvih</trd>,
    <trd>zv&yacute;&scaron;enie</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>zdvih</trd>,
    <trd>zdvi&zcaron;en&iacute;</trd>,
    <trd>zv&yacute;&scaron;en&iacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x456;&#x434;&#x439;&#x43E;&#x43C;</trd>,
    <trd>&#x43F;&#x456;&#x434;&#x43D;&#x44F;&#x442;&#x442;&#x44F;</trd>,
    <trd>&#x43F;&#x456;&#x434;&#x43D;&#x456;&#x43C;&#x430;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43F;&#x456;&#x434;&#x432;&#x438;&#x449;&#x435;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="lev.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="lev.0igxi.altigxi">
    <dif>
      Movi&gcirc;i supren; alti&gcirc;i:
      <ekz>
        aglo, aeroplano, fumo <tld/>i&gcirc;as alten;
      </ekz>
      <ekz>
        <tld/>i&gcirc;i en la &ccirc;ielon
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 30:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;i al la &ccirc;ielo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;is la terura vento de l' dezerto
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        suno, tago, matenru&gcirc;o <tld/>i&gcirc;as
        <fnt><bib>MT</bib> <lok>&Sa1; 9:26</lok></fnt>;
      </ekz>
      <ekz>
        la kolero <tld/>i&gcirc;is al lia kapo
        <fnt><bib>Far3</bib> <lok>&ccirc;ap. 9</lok></fnt>;
      </ekz>
      <ekz>
        lumo <tld/>i&gcirc;anta<fnt><bib>MT</bib> <lok>&Sen;
        4:18</lok></fnt>;
      </ekz>
      <ekz>
        birdo for<tld/>i&gcirc;as<fnt><bib>MT</bib> <lok>&Sen;
        26:2?</lok></fnt>
        <klr>(forflugas)</klr>;
      </ekz>
      <ekz>
        kiam la roso for<tld/>i&gcirc;is<fnt><bib>MT</bib> <lok>Eliro
        16:14</lok></fnt>;
      </ekz>
      <ekz>
        de la <ind>sun<tld/>i&gcirc;ejo</ind> &gcirc;is la sunsubirejo
        Mia nomo estos granda inter la nacioj
        <fnt><bib>MT</bib> <lok>&Mal; 1:11</lok></fnt>.
        <trd lng="fr">levant<klr tip="ind"> (point
        cardinal)</klr></trd>
        <trd lng="hu">napkelet</trd>
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd><klr>sich </klr>heben</trd>,
      <trd><klr>sich </klr>erheben</trd>,
      <trd>aufsteigen</trd>,
      <trd>aufgehen</trd>
    </trdgrp>
    <trd lng="es">elevarse</trd>
    <trdgrp lng="hu">
      <trd>emelkedik</trd>,
      <trd>felemelkedik</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>opstijgen</trd>,
      <trd>opstaan</trd>
    </trdgrp>
  </snc>
  <snc mrk="lev.0igxi.starigxi">
    <dif>
      Rekte ekstari:
      <ekz>
        <tld/>i&gcirc;i de la se&gcirc;o
        <fnt><bib>Far3</bib> <lok>&ccirc;ap. 8a</lok></fnt>;
      </ekz>
      <ekz>
        li matene <tld/>i&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 6:38</lok>
        </fnt>;
      </ekz>
      <ekz>
        montar&ccirc;eno abrupte <tld/>i&gcirc;as;
      </ekz>
      <ekz>
        granda nacio <tld/>i&gcirc;os <klr>(alvenos)</klr> de la randoj 
        de la tero
        <fnt><bib>MT</bib> <lok>&Jer; 6:22</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        gigante nun anta&ubreve; mi <tld/>i&gcirc;as via bildo
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la urbo rapide re<tld/>i&gcirc;is el ruinoj.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>aufstehen</trd>,
      <trd><klr>sich </klr>erheben</trd>,
      <trd><klr>sich </klr>aufrichten</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>fel&aacute;ll</trd>,
      <trd>felemelkedik</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>zich <ind>oprichten</ind></trd>,
      <trd>verrijzen</trd>
    </trdgrp>
  </snc>
  <snc mrk="lev.0igxi.komencigxi">
    <dif>
      Komenci&gcirc;i:
      <ekz>
        <tld/>i&gcirc;is akra vento
        <fnt><bib>Fab1</bib> <lok>Galo&scirc;oj de feli&ccirc;o</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;is murmuro, krio;
      </ekz>
      <ekz>
        subita bruo <tld/>i&gcirc;is en la domo
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        malfeli&ccirc;o kaj peko <tld/>i&gcirc;as sen
        veko
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>aufkommen</trd>,
      <trd>anheben</trd>,
      <trd>beginnen</trd>,
      <trd><klr>sich </klr>erheben</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>t&aacute;mad</trd>,
      <trd>kerekedik</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>oprijzen</trd>,
      <trd>verschijnen</trd>
    </trdgrp>
  </snc>
  <snc mrk="lev.0igxi.pliigxi">
    <dif>
      Plii&gcirc;i je ia grado:
      <ekz>
        lia vo&ccirc;o <tld/>i&gcirc;is pro kolero.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd><klr>sich </klr>heben</trd>,
      <trd><klr>sich </klr>erheben</trd>,
      <trd>h&ouml;her werden</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>er&odblac;s&ouml;dik</trd>,
      <trd>n&ouml;vekszik</trd>
    </trdgrp>
    <trd lng="nl">zich <ind>verheffen</ind></trd>
  </snc>
  <snc mrk="lev.0igxi.nobligxi">
    <dif>
      Spirite, morale plialti&gcirc;i:
      <ekz>
        liaj pensoj <tld/>i&gcirc;is <klr>(nobli&gcirc;is)</klr> per la 
        meditado;
      </ekz>
      <ekz>
        la homaro <tld/>i&gcirc;is super la bestoj;
      </ekz>
      <ekz>
        <tld/>i&gcirc;i super la kvereloj de l' &ccirc;iutaga vivo;
      </ekz>
      <ekz>
        <tld/>i&gcirc;i &gcirc;is tia situacio, ke
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>ansteigen</trd>,
      <trd><klr>sich </klr>erheben</trd>,
      <trd>steigen</trd>,
    </trdgrp>
    <trd lng="nl">uitstijgen</trd>
  </snc>
  <snc mrk="lev.0igxi.ataki">
    <dif>
      <refgrp tip="dif">
        <ref cel="agres.0i">Agresi</ref>,
        <ref cel="atak.0i">ataki</ref>
      </refgrp>:
      <ekz>
        la Eternulo, via Dio, faros, ke viaj malamikoj,
        kiuj <tld/>i&gcirc;os kontra&ubreve; vin, estos frapitaj anta&ubreve; vi
        <fnt><bib>MT</bib> <lok>&Rea; 28:7</lok></fnt>;
      </ekz>
      <ekz>
        kaj kiam ili estis sur la kampo, <nom>Kain</nom> <tld/>i&gcirc;is
        kontra&ubreve; sian fraton <nom>Habel</nom> kaj mortigis lin
        <fnt><bib>MT</bib> <lok>&Gen; 4:8</lok></fnt>.          
      </ekz>
    </dif>
    <trd lng="ru">&c_v;&c_o;&c_s;&c_s;&c_t;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="lev.0igxi.ribeli">
    <dif>
      <ref tip="dif" cel="ribel.0i">Ribeli</ref>:
      <ekz>
        <tld/>i&gcirc;u, proletar' de l' tero,
        <tld/>i&gcirc;u, sklavoj de malsat'!
        <fnt>
          <aut>E. Pottier, trad. K. Kalocsay</aut>:
          <vrk>Internacio</vrk></fnt>.
      </ekz>
    </dif>
    <trd lng="en">rise<klr> (against)</klr></trd>
    <trd lng="ru">&c_v;&c_o;&c_s;&c_s;&c_t;&c_a;&c_t;&c_mol;</trd>
