<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: albumi1.xml,v 1.17 2018/10/12 16:10:19 revo Exp $">
<kap>
  <rad>albuminuri</rad>/o
  <fnt>Z</fnt>
</kap>

<drv mrk="albumi1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      &Ccirc;eesto de <ref cel="albumi.0o">albumino</ref> en la urino (=
      <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>
      de <nom>Bright</nom>):
      <ekz>
        eklampsio <klr>[&#8230;]</klr> estas subite aperanta malsano ofta 
        dum la lasta tempo de gravedeco a&ubreve; tuj poste kaj karakterizata
        per konvulsiaj, epilepsiaj spasmoj kaj per <tld/>o, do apero de albumino en la urino
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=537824">Eklampsio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_b;&c_u;&c_m;&c_ib;&c_n;&c_u;&c_r;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_a;&c_l;&c_b;&c_u;&c_m;&c_i;&c_n;&c_u;&c_r;&c_i;&c_ja;</trd>
  <trd lng="de">Albuminurie</trd>
  <trd lng="en">albuminuria</trd> 
  <trd lng="fa">&a_A_madda;&a_l;&a_b;&a_w;&a_m;&f_y;&a_n;&a_w;&a_r;&f_y;</trd>
  <trd lng="fr">albuminurie</trd>
  <trdgrp lng="hu">
    <trd>feh&eacute;rjevizel&eacute;s</trd>,
    <trd>albuminvizel&eacute;s</trd>,
    <trd>albuminuria</trd>
  </trdgrp>
  <trd lng="id">albuminuria</trd>
  <trdgrp lng="nl">
    <trd>albuminurie</trd>,
    <trd>ziekte van <ind>Bright</ind></trd>
  </trdgrp>
  <trd lng="pl">albuminuria</trd>
  <trd lng="pt">albumin&uacute;ria</trd>
  <trd lng="ru">&c_a;&c_l;&c_mol;&c_b;&c_u;&c_m;&c_i;&c_n;&c_u;&c_r;&c_i;&c_ja;</trd>
</drv>
</art>
