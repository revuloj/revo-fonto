<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: iriz.xml,v 1.14 2025/03/30 17:18:22 revo Exp $">
<kap>
  <rad>iriz</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="iriz.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="iriz.0i.KOMUNE">
    <dif>
      Alternigi la kolorojn la&ubreve; movoj de la lumo, de la
      observanto:
      <ekz>
        prefere kombinu la kolorojn kiuj reciproke <tld/>u
        <fnt>
        <aut>T. Plicka, trad. L. Bor&ccirc;i&ccirc;</aut>,
        <vrk>Ru&gcirc;aj pasko-ovoj</vrk>,
        <lok><url ref="&SCE;">
        soc.culture. esperanto</url>, 1997-03-28</lok>
        </fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="muar.0i">muari</ref>
    </refgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>&#265;ielarke kolori</ke>.
  </rim>

  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_c;&c_a; <klr>(&c_f;&c_a;&c_r;&c_b;&c_a;&c_m;&c_ib;, &c_n;&c_ib;&c_b;&c_y; &c_v;&c_ja;&c_s;&c_jo;&c_l;&c_k;&c_a;)</klr></trd>
  <trdgrp lng="cs">
    <trd>b&#253;t duhov&#283; zbarven&#253;</trd>,
    <trd>dou&#382;kovat</trd>,
    <trd>irizovat</trd>
  </trdgrp>
  <trd lng="de">irisieren</trd>
  <trd lng="fr">iriser</trd>
  <trd lng="ja">&#34425;&#33394;&#12395;&#24425;&#12427;  <pr>&#12395;&#12376;&#12356;&#12429;&#12395;&#12356;&#12429;&#12393;&#12427;</pr></trd>
  <trdgrp lng="pl">
    <trd>iryzowa&cacute;</trd>,
    <trd>mieni&cacute; si&eogonek; barwami t&eogonek;czy</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>iriar(-se)</trd>,
    <trd>irisar(-se)</trd>,
    <trd>matizar(-se)</trd>
  </trdgrp>
  <trd lng="sk">meni&tcaron; farbu ako d&#250;ha</trd>
  <trd lng="uk">&c_r;&#1086;&c_z;&c_c;&c_v;&c_ib;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i; &c_v;&c_s;&c_ib;&#1084;&c_a; &#1082;&#1086;&#1083;&#1100;&#1086;&c_r;&c_a;&#1084;&c_i;
  &c_v;&c_je;&c_s;&c_je;&#1083;&#1082;&c_i;</trd>
</drv>

<drv mrk="iriz.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="iriz.0a.KOMUNE">
    <dif>
      Kiu <tld/>as, kies koloroj &scirc;ajne movi&gcirc;as:
      <ekz>
        inter <tld/>aj makuloj de <klr>...</klr> oleo
        <fnt>
        <aut>Lubovj Lukina, E&ubreve;geno Lukin, trad. Jurij Finkel
        </aut>,
        <vrk>La misiistoj</vrk>,
        <lok><url ref="http://www.esperanto.mv.ru/Lukin/mis2.html">parto
        2a</url></lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_l;&c_ib;&c_v;&c_ib;&c_s;&c_t;&c_y; <klr>(&c_f;&c_a;&c_r;&c_b;&c_a;&c_m;&c_ib;)</klr></trd>
  <trd lng="cs">duhov&#283; zbarven&#253;</trd>
  <trd lng="de">irisierend</trd>
  <trd lng="fr">iris&eacute;</trd>
  <trdgrp lng="ja">
    <trd>&#34425;&#33394;&#12398;  <pr>&#12395;&#12376;&#12356;&#12429;&#12398;</pr></trd>,
    <trd>&#34425;&#33394;&#12395;&#36637;&#12367;  <pr>&#12395;&#12376;&#12356;&#12429;&#12395;&#12363;&#12364;&#12420;&#12367;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>iryzuj&aogonek;cy</trd>,
    <trd>mieni&aogonek;cy si&eogonek; barwami t&eogonek;czy</trd>
    <trd>t&eogonek;czowy</trd>
  </trdgrp>
  <trd lng="sk">d&#250;hov&#253;</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_a;&c_j;&c_d;&c_u;&c_zh;&#1085;&c_i;&c_j;</trd>,
    <trd>&#1087;&c_je;&c_r;&c_je;&#1083;&c_i;&c_v;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="iriz.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="iriz.0o.KOMUNE">
    <dif>
      &Scirc;ajne movi&gcirc;antaj koloroj.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_l;&c_ib;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je; <klr tip="amb">(&c_k;&c_o;&c_l;&c_je;&c_r;&c_a;&c_w;)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#34425;&#33394;&#21177;&#26524;  <pr>&#12395;&#12376;&#12356;&#12429;&#12371;&#12358;&#12363;</pr></trd>,
    <trd>&#34425;&#33394;&#12398;&#36637;&#12365;  <pr>&#12395;&#12376;&#12356;&#12429;&#12398;&#12363;&#12364;&#12420;&#12365;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>iryzacja</trd>,
    <trd>t&eogonek;czowa gra barw</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#1087;&c_je;&c_r;&c_je;&#1083;&c_i;&c_v;&c_a;&#1085;&#1085;&#1103; &#1082;&#1086;&#1083;&#1100;&#1086;&c_r;&c_a;&#1084;&c_i;
    &c_v;&c_je;&c_s;&c_je;&#1083;&#1082;&c_i;</trd>,
    <trd>&c_ib;&c_r;&c_i;&c_z;&c_a;&c_c;&c_ib;&#1103;</trd>
  </trdgrp>
</drv>
</art>
