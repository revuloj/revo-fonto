<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: iriz.xml,v 1.10 2022/04/30 08:42:42 revo Exp $">
<kap>
<rad>iriz</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="iriz.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="iriz.0i.KOMUNE">
    <dif>
      Alternigi la kolorojn la&ubreve; movoj de la lumo, de la
      observanto:
      <ekz>
        prefere kombinu la kolorojn kiuj reciproke <tld/>u
        <fnt>
        <aut>T. Plicka, trad. L. Bor&ccirc;i&ccirc;</aut>,
        <vrk>Ru&gcirc;aj pasko-ovoj</vrk>,
        <lok><url ref="&SCE;">
        soc.culture. esperanto</url>, 1997-03-28</lok>
        </fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="muar.0i">muari</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>b&#xFD;t duhov&#x11B; zbarven&#xFD;</trd>,
    <trd>dou&#x17E;kovat</trd>,
    <trd>irizovat</trd>
  </trdgrp>
  <trd lng="de">irisieren</trd>
  <trd lng="fr">iriser</trd>
  <trdgrp lng="pl">
    <trd>iryzowa&cacute;</trd>,
    <trd>mieni&cacute; si&eogonek; barwami t&eogonek;czy</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>iriar(-se)</trd>,
    <trd>irisar(-se)</trd>,
    <trd>matizar(-se)</trd>
  </trdgrp>
  <trd lng="sk">meni&tcaron; farbu ako d&#xFA;ha</trd>
  <trd lng="uk">&c_r;&#x43E;&c_z;&c_c;&c_v;&c_ib;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i; &c_v;&c_s;&c_ib;&#x43C;&c_a; &#x43A;&#x43E;&#x43B;&#x44C;&#x43E;&c_r;&c_a;&#x43C;&c_i;
  &c_v;&c_je;&c_s;&c_je;&#x43B;&#x43A;&c_i;</trd>
</drv>

<drv mrk="iriz.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="iriz.0a.KOMUNE">
    <dif>
      Kiu <tld/>as, kies koloroj &scirc;ajne movi&gcirc;as:
      <ekz>
        inter <tld/>aj makuloj de <klr>...</klr> oleo
        <fnt>
        <aut>Lubovj Lukina, E&ubreve;geno Lukin, trad. Jurij Finkel
        </aut>,
        <vrk>La misiistoj</vrk>,
        <lok><url ref="http://www.esperanto.mv.ru/Lukin/mis2.html">parto
        2a</url></lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">duhov&#x11B; zbarven&#xFD;</trd>
  <trd lng="de">irisierend</trd>
  <trd lng="fr">iris&eacute;</trd>
  <trdgrp lng="pl">
    <trd>iryzuj&aogonek;cy</trd>,
    <trd>mieni&aogonek;cy si&eogonek; barwami t&eogonek;czy</trd>
    <trd>t&eogonek;czowy</trd>
  </trdgrp>
  <trd lng="sk">d&#xFA;hov&#xFD;</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_a;&c_j;&c_d;&c_u;&c_zh;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_je;&#x43B;&c_i;&c_v;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="iriz.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="iriz.0o.KOMUNE">
    <dif>
      &Scirc;ajne movi&gcirc;antaj koloroj.
    </dif>
  </snc>
  <trdgrp lng="pl">
    <trd>iryzacja</trd>,
    <trd>t&eogonek;czowa gra barw</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_je;&c_r;&c_je;&#x43B;&c_i;&c_v;&c_a;&#x43D;&#x43D;&#x44F; &#x43A;&#x43E;&#x43B;&#x44C;&#x43E;&c_r;&c_a;&#x43C;&c_i;
    &c_v;&c_je;&c_s;&c_je;&#x43B;&#x43A;&c_i;</trd>,
    <trd>&c_ib;&c_r;&c_i;&c_z;&c_a;&c_c;&c_ib;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
