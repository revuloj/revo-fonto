<art mrk="$Id: iridac.xml,v 1.14 2023/11/19 10:22:14 revo Exp $">
<kap>
  <rad>iridac</rad>/oj
</kap>
<drv mrk="iridac.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref>  de <ref tip="malprt" cel="kotile.unu0aj">unukotiledonaj</ref>
      plantoj (<trd lng="la">Iridaceae</trd>):
      <ekz>
        <ref tip="prt" cel="krokus.0o">krokuso</ref>
        estas genro el la planta familio <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=139165">Krokuso</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_s;&c_a;&c_ch;&c_o;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">irid&agrave;cies</trd>
  <trd lng="de">Schwertliliengew&auml;chse</trd>
  <trd lng="en">Iridaceae</trd>
  <trd lng="fr">iridac&eacute;es</trd>
  <trd lng="hu">n&odblac;sziromf&eacute;l&eacute;k</trd>
  <trd lng="nl">lissenfamilie</trd>
  <trd lng="pt">irid&aacute;ceas</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_s;&c_a;&c_t;&c_i;&c_k;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_i;&c_r;&c_i;&c_s;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="uk">&c_p;&c_ib;&c_v;&c_n;&c_i;&c_k;&c_o;&c_v;&c_ib;</trd>
  <trdgrp lng="zh">
    <trd>&#x9CF6;&#x5C3E;&#x82B1; <pr>yu&#x101;nw&#x11B;ihu&#x101;</pr></trd>,
    <trd>&#x9CF6;&#x5C3E;&#x79D1; <pr>yu&#x101;nw&#x11B;ik&#x113;</pr></trd>
  </trdgrp>
</drv>
</art>
