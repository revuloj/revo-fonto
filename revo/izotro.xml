<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>izotrop</rad>/a
</kap>
<drv mrk="izotro.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <uzo tip="klr">(pri korpo)</uzo>
    <dif>
      Posedanta la samajn fizikajn proprecojn en &ccirc;iuj direktoj.
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_z;&c_a;&c_t;&c_r;&c_o;&c_p;&c_n;&c_y;</trd>
  <trd lng="de">isotrop</trd>
  <trd lng="fr">isotrope</trd>
  <trd lng="es">isotr&oacute;pico</trd>
  <trdgrp lng="hu">
    <trd>izotrop</trd>,
    <trd>izotropikus</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_z;&c_o;&c_t;&c_r;&c_o;&c_p;&c_n;&c_y;&c_j;</trd>
  <trd lng="nl">isotroop</trd>
</drv>

<drv mrk="izotro.mal0a">
  <kap>mal<tld/>a</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <uzo tip="klr">(pri korpo)</uzo>
    <dif>
      Ne posedanta la samajn fizikajn proprecojn en &ccirc;iuj direktoj.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_ib;&c_z;&c_a;&c_t;&c_r;&c_o;&c_p;&c_n;&c_y;</trd>
  <trd lng="de">anisotrop</trd>
  <trd lng="fr">anisotrope</trd>
  <trd lng="es">anisotr&oacute;pico</trd>
  <trdgrp lng="hu">
    <trd>anizotrop</trd>,
    <trd>anizotropikus</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_n;&c_i;&c_z;&c_o;&c_t;&c_r;&c_o;&c_p;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="izotro.0eco">
  <kap><tld/>eco</kap>
  <uzo tip="fak">FIZ</uzo>
  <fnt><bib>KrED</bib></fnt>
  <fnt><bib>FzT</bib></fnt>
  <snc>
    <uzo tip="klr">(pri korpo)</uzo>
    <dif>
      Kvalito de iu korpo posedanta la samajn fizikajn proprecojn en
      &ccirc;iuj direktoj.
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_z;&c_a;&c_t;&c_r;&c_o;&c_p;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="de">Isotropie</trd>
  <trd lng="en">isotropy</trd>
  <trd lng="fr">isotropie</trd>
  <trd lng="hu">izotr&oacute;pia</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_o;&c_t;&c_r;&c_o;&c_p;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_z;&c_o;&c_t;&c_r;&c_o;&c_p;&c_i;&c_ja;</trd>
  </trdgrp>  
</drv>
</art>
