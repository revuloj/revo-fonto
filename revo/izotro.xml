<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <rad>izotrop</rad>/a
</kap>
<drv mrk="izotro.0a">
   <kap><tld/>a</kap>
   <uzo tip="fak">FIZ</uzo>
   <snc>
     <uzo tip="klr">(pri korpo)</uzo>
     <dif>
       Posedanta la samajn fizikajn proprecojn en &ccirc;iuj direktoj.
     </dif>
   </snc>
   <trd lng="de">isotrop</trd>
   <trd lng="fr">isotrope</trd>
   <trd lng="es">isotr&oacute;pico</trd>
   <trd lng="ru">&c_i;&c_z;&c_o;&c_t;&c_r;&c_o;&c_p;&c_n;&c_y;&c_j;</trd>
   <trd lng="nl">isotroop</trd>
</drv>
<drv mrk="izotro.mal0a">
   <kap>mal<tld/>a</kap>
   <uzo tip="fak">FIZ</uzo>
   <snc>
     <uzo tip="klr">(pri korpo)</uzo>
     <dif>
       Ne posedanta la samajn fizikajn proprecojn en &ccirc;iuj direktoj.
     </dif>
   </snc>
   <trd lng="de">anisotrop</trd>
   <trd lng="fr">anisotrope</trd>
   <trd lng="es">anisotr&oacute;pico</trd>
</drv>
<drv mrk="izotro.0eco">
   <kap><tld/>eco</kap>
   <uzo tip="fak">FIZ</uzo>
   <fnt><bib>KrED</bib></fnt>
   <fnt><bib>FzT</bib></fnt>
   <snc>
     <uzo tip="klr">(pri korpo)</uzo>
     <dif>
       Kvalito de iu korpo posedanta la samajn fizikajn proprecojn en 
&ccirc;iuj direktoj.
     </dif>
   </snc>
   <trd lng="de">Isotropie</trd>
   <trd lng="en">isotropy</trd>
   <trd lng="fr">isotropie</trd>
</drv>
</art>
