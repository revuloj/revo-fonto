<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mek.xml,v 1.14 2024/01/25 20:10:36 revo Exp $">
<kap>
  <rad>mek</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="mek.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="mek.0.SON">
    <dif>
      <ref tip="lst" cel="blek.0o" lst="voko:blekoj">Bleko</ref>
      de kapro:
      <ekz>
        la kapro ekparolis: <tld/>-<tld/>
        <fnt>
          <aut>Vizi L.</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/eszperanto/361911">
          La leporo kiel interpretisto</url></vrk>,
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_je;&c_je;!</trd>
  <trd lng="de">meckmeckmeck</trd>
  <trd lng="fr">b&ecirc;&ecirc;<klr> (ch&egrave;vre)</klr></trd>
</drv>

<drv mrk="mek.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="mek.0i.ZOO">
    <dif>
      <ref tip="super" cel="blek.0i.beste">Bleki</ref> kiel kapro:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &leftquot;he - he!&rightquot; la gnomo en &gcirc;ojo <tld/>as
        <fnt>
          <aut>A. Garborg, trad. O. Tangerud</aut>,
          <vrk><url ref="&Gutenberg;files/24292/24292-0.txt">
          La Montarino</url></vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_l;&c_ja;&c_ja;&c_c;&c_mol; <klr>(&c_p;&c_r;&c_a; &c_k;&c_a;&c_z;&c_u;)</klr></trd>,
    <trd>&c_m;&c_je;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>belar</trd>,
    <trd>esbiegar</trd>
  </trdgrp>
  <trd lng="cs">me&ccaron;et</trd>
  <trd lng="es">balar</trd>
  <trd lng="de">meckern</trd>
  <trdgrp lng="fr">
    <trd>b&ecirc;ler<klr tip="amb"> (ch&egrave;vre)</klr></trd>,
    <trd>chevrotter</trd>
  </trdgrp>
  <trd lng="ja">&#x30E1;&#x30FC;&#x3068;&#x9CF4;&#x304F;  <pr>&#x30E1;&#x30FC;&#x3068;&#x306A;&#x304F;</pr></trd>
  <trd lng="pl">mecze&cacute;</trd>
  <trd lng="sk">me&ccaron;a&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_je;&#x43A;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_je;&#x43A;&c_a;&c_t;&c_i; (&#x43F;&c_r;&#x43E; &#x43A;&#x43E;&c_z;&c_u;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7F8B; <pr>m&#x1D0;</pr></trd>,
    <trd>&#x8288; <pr>m&#x1D0;</pr></trd>,
    <trd>&#x7F8A;&#x53EB; <pr>y&#xE1;ngji&#xE0;o</pr></trd>
  </trdgrp>
</drv>
</art>

