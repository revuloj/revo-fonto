<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mek.xml,v 1.10 2022/05/01 12:05:12 revo Exp $">
<kap>
  <rad>mek</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="mek.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="mek.0.SON">
    <dif>
      <ref tip="lst" cel="blek.0o" lst="voko:blekoj">Bleko</ref>
      de kapro:
      <ekz>
        la kapro ekparolis: <tld/>-<tld/>
        <fnt>
          <aut>Vizi L.</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/eszperanto/361911">
          La leporo kiel interpretisto</url></vrk>,
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">meckmeckmeck</trd>
  <trd lng="fr">b&ecirc;&ecirc;<klr> (ch&egrave;vre)</klr></trd>
</drv>

<drv mrk="mek.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="mek.0i.ZOO">
    <dif>
      <ref tip="super" cel="blek.0i.beste">Bleki</ref> kiel kapro:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &leftquot;he - he!&rightquot; la gnomo en &gcirc;ojo <tld/>as
        <fnt>
          <aut>A. Garborg, trad. O. Tangerud</aut>,
          <vrk><url ref="&Gutenberg;files/24292/24292-0.txt">
          La Montarino</url></vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>belar</trd>,
    <trd>esbiegar</trd>
  </trdgrp>
  <trd lng="cs">me&ccaron;et</trd>
  <trd lng="es">balar</trd>
  <trd lng="de">meckern</trd>
  <trdgrp lng="fr">
    <trd>b&ecirc;ler<klr tip="amb"> (ch&egrave;vre)</klr></trd>,
    <trd>chevrotter</trd>
  </trdgrp>
  <trd lng="pl">mecze&cacute;</trd>
  <trd lng="sk">me&ccaron;a&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x435;&#x43A;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x431;&#x435;&#x43A;&#x430;&#x442;&#x438; (&#x43F;&#x440;&#x43E; &#x43A;&#x43E;&#x437;&#x443;)</trd>
  </trdgrp>
</drv>
</art>

