<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: signatur.xml,v 1.4 2023/01/13 10:33:03 afido Exp $">
    <kap>
      <rad>signatur</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="signatur.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Distinga <ref tip="super" cel="sign.0o.KOMUNE">signo</ref> a&ubreve; signokombino postlasita sur io, 
      indikanta &gcirc;ian originon:
      <ekz>
        <tld/>o de artisto, libro, retpo&scirc;to, viruso;
      </ekz>
      <ekz>
        biologia, genetika, &hcirc;emia <tld/>o;
      </ekz>
      <ekz>
        la <tld/>o estas gravurita sur la <klr>[klingo]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=80150"
              >Japana glavo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la 1an de januaro aperis la unua semajna kajero de &leftquot;Civitano&rightquot;, <tld/>ita de
        redaktoro <nom>Dostojevskij</nom>
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 5 (55)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="mark.0o.signo">marko<sncref/></ref>,
      <ref cel="kon.re0ilo.karakterizajxo">rekonilo<sncref/></ref>,
      <ref cel="specif.0o">specifa&jcirc;o</ref>,
      <ref cel="trajt.0o.karakteriza">trajto<sncref/></ref>
    </refgrp>
    <refgrp tip="sub">
      <ref cel="kod.0o.itajxo">kodo</ref>,
      <ref cel="monogr1.0o">monogramo</ref>,
      <ref cel="paraf.0o">parafo</ref>,
      <ref cel="stamp.0o">stampo</ref>,
      <ref cel="skrib.sub0o.JUR">subskribo<sncref/></ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="distin.0ilo">distingilo</ref>,
      <ref cel="sign.prem0o">premsigno</ref>,
      <ref cel="sigel.0o">sigelo</ref>,
      <ref cel="spur.0o.postsigno">spuro</ref>
    </refgrp>
    <trd lng="be">&c_s;&c_y;&c_g;&c_n;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Erkennungsmerkmal</trd>,
      <trd>K&uuml;rzel</trd>,
      <trd>Signatur</trd>,
      <trd>charakteristisches <ind>Muster</ind></trd>,
      <trd>Marker</trd>,
      <trd>Charakteristikum</trd>,
      <trd>Abdruck</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>signature</trd>,
      <trd>identifier</trd>,
      <trd>characteristic <ind>pattern</ind></trd>
    </trdgrp>
    <trd lng="fr">signature</trd>
  </snc>
</drv>

  </art>

