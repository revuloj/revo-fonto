<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: politeknik.xml,v 1.17 2025/08/27 18:00:21 revo Exp $">
<kap>
  <rad>politeknik</rad>/o
<fnt><bib>JED</bib></fnt></kap>
<drv mrk="politeknik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PED</uzo>
  <snc mrk="politeknik.0o.fako">
    <fnt><bib>Fab4</bib></fnt>
    <dif>
      Komuna nomo de instruaj <ref tip="super" cel="fak.0o">fako</ref>j
      en teknika lernejo edukantaj pri bazaj in&gcirc;enieraj scioj:
      <ekz>
        lernanto de la <tld/>a lernejo
        <fnt>
          <bib>Fab4</bib>
          <lok>Pupludisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>isto komencis sian prelegon kaj siajn eksperimentojn
        <fnt>
          <bib>Fab4</bib>
          <lok>Pupludisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiun superan lernejon vi estas fininta?
        &ndash; la petrogradan <tld lit="P"/>an Instituton
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Polytechnik</trd>,
      <trd>Arbeitslehre</trd>
    </trdgrp>
  </snc>
  <snc mrk="politeknik.0o.lernejo">
    <fnt><bib>SPV</bib></fnt>
    <dif>
      Teknika <ref tip="super" cel="lern.0ejo">lernejo</ref>:
      <ekz>
        pluraj kursoj devige jam haveblaj nur en la angla lingvo, anka&ubreve; en iuj el la
        plej gravaj <tld/>oj (Milano, Torino)
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011257.php">En rubujon, kulturo kaj lingvo de
          Italio</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>polit&egrave;cnic</trd>,
      <trd>institut polit&egrave;cnic</trd>
    </trdgrp>
    <trd lng="cs">polytechnika</trd>
    <trd lng="de">Polytechnische Oberschule</trd>
    <trd lng="fa">&f_p;&a_l;&f_y;&zwnj;&a_t;&f_k;&a_n;&f_y;&f_k;</trd>
    <trd lng="fr">polytechnique<klr> (subst.)</klr></trd>
    <trdgrp lng="nl">
      <trd>polytechnicum</trd>,
      <trd>polytechnische school</trd>
    </trdgrp>
    <trd lng="pl">politechnika</trd>
    <trd lng="pt">polit&eacute;cnica</trd>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_i;&c_t;&c_je;&c_h;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;
    &c_i;&c_n;&c_s;&c_t;&c_i;&c_t;&c_u;&c_t;</trd>
    <trd lng="sk">polytechnika</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_ib;&c_t;&c_e;&c_h;&c_n;&c_ib;&c_k;&c_u;&c_m;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_ib;&c_t;&c_e;&c_h;&c_n;&c_ib;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_ib;&c_t;&c_e;&c_h;&c_n;&c_ib;&c_ch;&c_n;&c_y;
    <ind>&c_ib;&c_n;&c_s;&c_t;&c_y;&c_t;&c_u;&c_t;</ind></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12509;&#12522;&#12486;&#12463;&#12491;&#12540;&#12463;</trd>,
    <trd>&#29702;&#24037;&#31185;&#22823;&#23398;  <pr>&#12426;&#12371;&#12358;&#12363;&#12384;&#12356;&#12364;&#12367;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_l;&c_ib;&c_t;&c_je;&c_h;&c_n;&c_ib;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_ib;&c_t;&c_je;&c_h;&c_n;&c_ib;&c_ch;&c_n;&c_i;&c_j;
    &c_ib;&c_n;&c_s;&c_t;&c_i;&c_t;&c_u;&c_t;/&c_u;&c_g;&c_ib;&c_v;&c_je;&c_r;&c_s;&c_i;&c_t;&c_je;&c_t;</trd>
  </trdgrp>
</drv>
</art>

