<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hafni.xml,v 1.24 2023/11/15 16:45:17 revo Exp $">
<kap>
  <rad>hafni</rad>/o 
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="hafni.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Hf</mlg>
  <snc mrk="hafni.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="72">elemento</ref> kun
      atomnumero 72, atompezo 178,49;
      ar&gcirc;entblanka transir<ref tip="super" cel="metal.0o">metalo</ref>:
      <ekz>
        <tld/>o estas toksa kemia elemento <klr>[&#x2026;]</klr> uzata en volframaj alojoj por
        elektrodoj kaj filamentoj, degelpunkto estas 2233&#x202F;&Celsius;, bolpunkto estas 4603&#x202F;&Celsius;,
        <tld/>o trovi&gcirc;is en 1922
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=24508">Hafnio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_f;&c_n;&c_ib;&c_j;</trd>
  <trd lng="bg">&c_h;&c_a;&c_f;&c_n;&c_i;&c_j;</trd>
  <trd lng="br">hafniom</trd>
  <trd lng="ca">hafni</trd>
  <trdgrp lng="cs">
    <trd>hahnium</trd>,
    <trd>nielsbohrium</trd>
  </trdgrp>
  <trd lng="de">Hafnium</trd>
  <trd lng="en">hafnium</trd>
  <trd lng="es">hafnio</trd>
  <trd lng="fa">&a_h;&a_A;&a_f;&a_n;&f_y;&a_m;</trd>
  <trd lng="fr">hafnium</trd>
  <trd lng="hu">hafnium</trd>
  <trd lng="id">hafnium</trd>
  <trd lng="it">afnio</trd>
  <trd lng="la">Hafnium</trd>
  <trd lng="nl">hafnium</trd>
  <trd lng="pl">hafn</trd>
  <trd lng="pt">h&aacute;fnio</trd>
  <trd lng="ru">&c_g;&c_a;&c_f;&c_n;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>Hf</trd>,
    <trd>hafnium</trd>
  </trdgrp>
  <trd lng="sv">hafnium</trd>
  <trd lng="uk">&#x433;&#x430;&#x444;&#x43D;&#x456;&#x439;</trd>
  <trd lng="zh">&#x927F; <pr>h&#x101;</pr></trd>
</drv>
</art>
