  <trdgrp lng="hu">
    <trd>akad&aacute;ly</trd>,
    <trd>g&aacute;t</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>belemmering</trd>,
    <trd>hindernis</trd>
  </trdgrp>
  <trd lng="pt">estorvo</trd>
