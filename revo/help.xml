<drv mrk="help.antauxmal0i">
  <kap>anta&ubreve;mal<tld/>i</kap>
  <fnt><bib>PIV1</bib>, <lok>artikolo &leftquot;preventi&rightquot;</lok></fnt>
  <snc>
    <dif>
      Neebligi, ke io malbona okazu, aperu.
    </dif>
    <refgrp tip="sin">
      <ref cel="preven.0i">preventi</ref>;
    </refgrp>
    <refgrp tip="vid">
      <ref cel="gard.antaux0i">anta&ubreve;gardi</ref>;
      Preska&ubreve;aj sinonimoj:
      <ref cel="for.antaux0igi">anta&ubreve;forigi</ref>,
      <ref cel="halt.antaux0igi">anta&ubreve;haltigi</ref>
    </refgrp>
  </snc>     
  <trd lng="br">diarbenn</trd>
  <trdgrp lng="fr">
    <trd>&eacute;viter<klr tip="ind"> (la survenue de)</klr></trd>,
    <trd>parer<klr tip="ind"> (&eacute;viter)</klr></trd>,
    <trd>pr&eacute;venir<klr tip="ind"> (emp&ecirc;cher la survenue)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>elh&aacute;r&iacute;t</trd>,
    <trd>megel&odblac;z <klr tip="amb">(pl. bajt)</klr></trd>
  </trdgrp>
  <trd lng="pt">prevenir</trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_o;&c_t;&c_v;&c_r;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="help.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="help.sen0a.iu">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Tia, ke mankas &ccirc;ia <tld/>o:
      <ekz>
        sensa&gcirc;aj kaj sen<tld/>aj bestoj
        <fnt><bib>EE</bib></fnt>;
      </ekz>
      <ekz>
        la kontra&ubreve;uloj ekstaras sen<tld/>e kaj ne scias, kion respondi
        <fnt><bib>EE</bib></fnt>;
      </ekz>
      <ekz>
        <klr>[la E-istoj]</klr> montradis en sia agado grandegan nelertecon
        kaj sen<tld/>econ
        <fnt><bib>EE</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_b;&c_je;&c_z;&c_d;&c_a;&c_p;&c_a;&c_m;&c_o;&c_zh;&c_n;&c_y;</trd>,
      <trd>&c_b;&c_ja;&c_s;&c_p;&c_o;&c_m;&c_a;&c_ch;&c_n;&c_y;</trd>
    </trdgrp>   
    <trdgrp lng="br">
      <trd>diharp</trd>,
      <trd>diskoazell</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>abandonn&eacute;</trd>,
      <trd>d&eacute;sempar&eacute;</trd>
    </trdgrp>
    <trd lng="hu">tehetetlen</trd>
    <trd lng="ru">&c_b;&c_je;&c_s;&c_p;&c_o;&c_m;&c_o;&c_shch;&c_n;&c_y;&c_j;</trd>
    <trd lng="nl">hulpeloos</trd>
    <trd lng="pt">desamparado<klr tip="ind"> (adj.)</klr></trd>
  </snc>
  <snc mrk="help.sen0a.io">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Tia, ke nenia <tld/>o povus &gcirc;in plibonigi:
      <ekz>
        tio okazis el simpla, sen<tld/>a neceseco<fnt>Z</fnt>.
      </ekz>
    </dif>      
    <trdgrp lng="br">
      <trd>diremed</trd>,
      <trd>dirapar<klr tip="ind"> (diremed)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>irr&eacute;m&eacute;diable</trd>,
      <trd>irr&eacute;parable</trd>
    </trdgrp>
    <trd lng="hu">kiv&eacute;dhetetlen</trd>
    <trd lng="pt">irremedi&aacute;vel</trd>
    <trdgrp lng="ru">
      <trd>&c_n;&c_je;&c_p;&c_o;&c_p;&c_r;&c_a;&c_v;&c_i;&c_m;&c_y;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_i;&c_z;&c_l;&c_je;&c_ch;&c_i;&c_m;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="de">hilflos</trd>
</drv>
