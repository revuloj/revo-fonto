<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ibis</rad>/o
</kap>
<drv mrk="ibis.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="ibised.0oj"><tld/>edoj</ref>
      (<trd lng="la">Threskiornis</trd>),
      kun longaj kruroj kaj longa kurba beko, man&gcirc;anta serpentojn
      kaj aliajn rampulojn, pro kio &gcirc;i estis rigardata kiel sankta de la
      antikvaj egiptoj.
    </dif>
  </snc>
  <trd lng="de">Ibis</trd>
  <trd lng="nl">ibis</trd>
  <trd lng="ru">&c_i;&c_b;&c_i;&c_s;</trd>
</drv>

</art>
