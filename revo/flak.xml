<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>flak</rad>/o
</kap>
<drv mrk="flak.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Malgranda kvanto da akvo a&ubreve; alia likva&jcirc;o, restanta en kava&jcirc;o:
      <ekz><tld/>oj sur vojo post pluvo</ekz>;
      <ekz>
       sen atenti la flakojn la homo kun la kapu&ccirc;o transiris la placeton
       <fnt><bib>MkM</bib>, <lok>P.55.</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Pf&uuml;tze</trd>,
     <trd>Lache</trd></trdgrp>
  <trd lng="fr">flaque</trd>
  <trd lng="nl">plas</trd>
  <trd lng="ru">&c_l;&c_u;&c_zh;&c_a;</trd>
</drv>
</art>

