<kap>
  <rad>flak</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="flak.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Malgranda kvanto da akvo a&ubreve; alia likva&jcirc;o, restanta
      en kava&jcirc;o:
      <ekz>
        <tld/>oj sur vojo post pluvo;
      </ekz>
      <ekz>
        sen atenti la <tld/>ojn la homo kun la kapu&ccirc;o transiris
        la placeton
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 25a, p. 261a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_u;&c_zh;&c_y;&c_n;&c_a;</trd>
  <trd lng="bg">&c_l;&c_o;&c_k;&c_v;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>toll</trd>,
    <trd>bassiol</trd>,
    <trd>bassa</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kalu&#x17E;</trd>,
    <trd>lou&#x17E;e</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Pf&uuml;tze</trd>,
    <trd>Lache</trd>
  </trdgrp>
  <trd lng="en">puddle</trd>
  <trd lng="es">charco</trd>
  <trd lng="fr">flaque</trd>
  <trd lng="hu">t&oacute;csa</trd>
  <trd lng="ja">&#x6C34;&#x305F;&#x307E;&#x308A;  <pr>&#x307F;&#x305A;&#x305F;&#x307E;&#x308A;</pr></trd>
  <trd lng="nl">plas</trd>
  <trd lng="pl">ka&lstroke;u&zdot;a</trd>
  <trd lng="pt">po&ccedil;a</trd>
  <trd lng="ru">&c_l;&c_u;&c_zh;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>kalu&#x17E;</trd>,
    <trd>ml&#xE1;ka</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&#x430;&#x43B;&#x44E;&#x436;&#x430;</trd>
  <trdgrp lng="zh">
    <trd>&#x6C34;&#x6D3C; <pr>shu&#x1D0;w&#x101;</pr></trd>,
    <trd>&#x548C;&#x6CE5;&#x6D46; <pr>h&#xE9;n&#xED;ji&#x101;ng</pr></trd>
  </trdgrp>
</drv>
</art>
