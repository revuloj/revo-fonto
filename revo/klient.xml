<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>klient</rad>/o
</kap>
<drv mrk="klient.0o">
  <kap><tld/>o</kap>
  <snc mrk="klient.0o.HIS">
  <uzo tip="fak">HIS</uzo>
    <dif>
      En romia antikvo, plebano sinmetinta sub protektado de patricio.
    </dif>
    <trd lng="en">client</trd>
    <trdgrp lng="fr">
      <trd>client<klr> (client&eacute;lisme antique)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_l;&c_i;&c_je;&c_n;&c_t;</trd>
  </snc>
  <snc mrk="klient.0o.KOME">
    <uzo tip="fak">KOME</uzo>
    <dif>
      Kutima <ref tip="super" cel="acxet.0anto">a&ccirc;etanto</ref>
      de komerca&jcirc;oj en iu butiko a&ubreve;
      de iu kompanio, de servoj de iu prizorganto (notario,
      advokato, kuracisto...):
      <ekz>
        <tld/>o de viandisto, de banejo, de restoracio;
      </ekz>
      <ekz>
        mult<tld/>a butiko.
      </ekz>
      <ekz>
        la ju&gcirc;istoj senkulpigis mian <tld/>on;
      </ekz>
      <ekz>
        <tld/>aro de dentisto.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Klient</trd>,
      <trd>Kunde</trd>,
      <trd>Patient <klr> (p.p. kuracisto)</klr></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>client</trd>,
      <trd>customer</trd>,
      <trd>patient <klr> (p.p. kuracisto)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>client</trd>,
      <trd>consommateur</trd>,
      <trd>patient<klr> (d'un m&eacute;decin)</klr></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>cli&euml;nt</trd>,
      <trd lng="nl">klant</trd>,
      <trd>pati&euml;nt</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_l;&c_i;&c_je;&c_n;&c_t;</trd>,
      <trd>&c_p;&c_a;&c_c;&c_i;&c_je;&c_n;&c_t; <klr> (p.p. kuracisto)</klr></trd>,
      <trd><klr>&c_p;&c_o;&c_s;&c_t;&c_o;&c_ja;&c_n;&c_n;&c_y;&c_j;</klr>
&c_p;&c_o;&c_k;&c_u;&c_p;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
    </trdgrp>
  </snc>
</drv>
</art>
