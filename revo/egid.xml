<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>egid</rad>/o
</kap>
<drv mrk="egid.0o">
  <kap><tld/>o</kap>
  <snc mrk="egid.0o.protekto">
    <ref tip="dif" cel="patron.0eco">patroneco</ref>
    <ekz>
      esti sub ies <tld/>o;
    </ekz>
    <ekz>
      absoluteco de katolikismo, proklamita per la 1a Vatikana Koncilio
      sub la <tld/>o de papo Pio 9a<fnt><aut>Gerrit Berveling</aut>:
      <vrk>Pri la dokumento &leftquot;Dominus Iesus&rightquot; de Vatikano</vrk>,
      <bib>Monato</bib><lok>jaro 2000a, numero 11a, p. 18a</lok></fnt>.
    </ekz>
    <trdgrp lng="fr">
      <trd>auspices<klr> (patronnage)</klr></trd>,
      <trd>protection<klr> (patronnage)</klr></trd>
    </trdgrp>
    <trd lng="pt">prote&ccedil;&atilde;o</trd>
  </snc>
  <snc mrk="egid.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      &Scirc;ildo de la greka diino de sa&gcirc;o kaj milito Palaso.
    </dif>
    <trdgrp lng="de">
      <trd>&Auml;gis</trd>,
      <trd>Aegis</trd>
    </trdgrp>
    <trd lng="en">aegis</trd>
  </snc>
  <trdgrp lng="es">
    <trd>&eacute;gida</trd>,
    <trd>egida</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schild</trd>,
    <trd>Schutz</trd>,
    <trd>&Auml;gide</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>aegis</trd>,
    <trd>sponsorship</trd>,
    <trd>auspices</trd>,
    <trd>protection</trd>
  </trdgrp>
  <trd lng="fr">&eacute;gide</trd>
  <trd lng="pt">&eacute;gide</trd>
  <trd lng="ru">&c_e;&c_g;&c_i;&c_d;&c_a;</trd>
</drv>
</art>
