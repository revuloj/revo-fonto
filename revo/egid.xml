<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: egid.xml,v 1.27 2023/11/07 19:22:22 revo Exp $">
  <kap>
      <ofc>1</ofc>
      <rad>egid</rad>/o
  </kap>

<drv mrk="egid.0o">
  <kap><tld/>o</kap>
  <snc mrk="egid.0o.MIT">
      <uzo tip="fak">MIT</uzo>
      <dif>
          &Scirc;ildo de Ze&ubreve;so, simbolo de protekto a&ubreve;
          kolero de la dioj:
          <ekz>
              se en liaj manoj sin trovus la <tld/>o de Ze&ubreve;so,
              se per unu ekbato li povus ekstermi tiun &ccirc;i tutan
              malkura&gcirc;an bestaron, li ekstermus &gcirc;in kaj li
              sola irus anta&ubreve;en
              <fnt>
                  <aut>Eube el Odeso</aut>: <vrk>Nokto</vrk>.
                  <lok>Fundamenta Krestomatio</lok>
              </fnt>.
          </ekz>
      </dif>
      <trdgrp lng="de">
          <trd>&Auml;gis</trd>,
          <trd>Aegis</trd>
      </trdgrp>
      <trd lng="en">aegis</trd>
      <trdgrp lng="hu">
          <trd>&eacute;gisz<klr tip="ind">
          (v&eacute;d&odblac;pajzs)</klr></trd>,
          <trd>v&eacute;d&odblac;pajzs</trd></trdgrp>
      <trd lng="pt">&eacute;gide</trd>
  </snc>
  <snc mrk="egid.0o.protekto">
      <uzo tip="fak">POL</uzo>
      <dif>
          Alta <ref tip="dif" cel="protek.0ado">protektado</ref>:
          <ekz>
              esti sub ies <tld/>o;
          </ekz>
          <ekz>
          absoluteco de katolikismo, proklamita per la 1a Vatikana
          Koncilio sub la <tld/>o de papo Pio 9a
          <fnt>
          <aut>Gerrit Berveling</aut>:
          <vrk>Pri la dokumento &leftquot;Dominus Iesus&rightquot; de
          Vatikano</vrk>,
          <bib>Monato</bib><lok>jaro 2000a, numero 11a, p. 18a</lok>
          </fnt>.
          </ekz>
      </dif>
      <trdgrp lng="fr">
          <trd>auspices<klr> (patronnage)</klr></trd>,
          <trd>protection<klr> (patronnage)</klr></trd>
      </trdgrp>
      <trdgrp lng="hu">
          <trd>&eacute;gisz</trd>,
          <trd>v&eacute;d&odblac;sz&aacute;rny</trd>,
          <trd>p&aacute;rtfog&aacute;s</trd>
      </trdgrp>
      <trdgrp lng="nl">
          <trd>auspici&euml;n</trd>,
          <trd>hoge <ind>bescherming</ind></trd>,
          <trd>beschermheerschap</trd>
      </trdgrp>
      <trd lng="pt">prote&ccedil;&atilde;o</trd>
  </snc>
  <trd lng="be">&c_e;&c_g;&c_ib;&c_d;&c_a;</trd>
  <trd lng="ca">&egrave;gida</trd>
  <trdgrp lng="cs">
    <trd>ochrana</trd>,
    <trd>z&aacute;&scaron;tita</trd>,
    <trd>&ccaron;estn&aacute; funkce s podporou</trd>,
    <trd>&scaron;t&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="de">
        <trd>Schild</trd>,
        <trd>Schutz</trd>,
        <trd>&Auml;gide</trd>
    </trdgrp>
  <trdgrp lng="en">
        <trd>aegis</trd>,
        <trd>sponsorship</trd>,
        <trd>auspices</trd>,
        <trd>protection</trd>
    </trdgrp>
  <trdgrp lng="es">
        <trd>&eacute;gida</trd>,
        <trd>egida</trd>
    </trdgrp>
  <trd lng="fr">&eacute;gide</trd>
  <trdgrp lng="ja">
    <trd>&#x30A2;&#x30A4;&#x30AE;&#x30B9;</trd>,
    <trd>&#x4FDD;&#x8B77;  <pr>&#x307B;&#x3054;</pr></trd>,
    <trd>&#x5F8C;&#x63F4;  <pr>&#x3053;&#x3046;&#x3048;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="pl">egida</trd>
  <trd lng="pt">&eacute;gide</trd>
  <trd lng="ru">&c_e;&c_g;&c_i;&c_d;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>ochrana</trd>,
    <trd>z&aacute;&scaron;tita</trd>
  </trdgrp>
  <trd lng="uk">&c_je;&c_g;&c_ib;&c_d;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x4E3B;&#x529E; <pr>zh&#x1D4;b&#xE0;n</pr></trd>,
    <trd>&#x57C3;&#x7678;&#x65AF; <pr>&#x101;igu&#x1D0;s&#x12B;</pr></trd>
  </trdgrp>
</drv>
</art>
