<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
-<rad>a&ccirc;</rad></kap>
<subart>
  <dif>
    Sufikso esprimanta malbonecon, malbonan kvaliton:  
  </dif>
  <snc>
    <ekz>
      mizera &ccirc;eval<tld/>o 
      <fnt><bib>Fab1</bib>, <lok>Sova&gcirc;aj cignoj</lok></fnt>;
    </ekz>
    <ekz>
      dom<tld/>o <klr>(malbela domo)</klr>;
    </ekz>
    <ekz>rigard<tld/>i kiel bovido
      <fnt><bib>PrV</bib></fnt> 
      <klr>(gapi, stultmiene rigardi)</klr></ekz>
    <ekz>
      babil<tld/>i <klr>(stulte, vante a&ubreve; malice paroli.)</klr>
    </ekz>
    <refgrp tip="sin">
      <ref cel="fi">fi</ref>.
    </refgrp>
    <trdgrp lng="de">
       <trd>Schei&szlig;-<klr> [pref]</klr></trd>,
       <trd>Mist-<klr> [pref]</klr></trd>,
       <trd>Drecks-<klr> [pref]</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>-asse<klr> (suffixe p&eacute;joratif)</klr></trd>,
      <trd>-&acirc;tre<klr> (suffixe p&eacute;joratif)</klr></trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>-aco<klr> (sufijo peyorativo)</klr></trd>,
      <trd>-ucho<klr> (sufijo peyorativo)</klr></trd>
    </trdgrp>
    <trd lng="vo">lu-</trd>
  </snc>
</subart>
<subart>
  <dif>
    Vortero memstara samsignifa: 
  </dif>
  <snc>
    <ekz>
      <tld/>a, senvalora vo&ccirc;o de publikulino;
    </ekz>
    <ekz>
      <tld/>a&jcirc;o;
    </ekz>
    <ekz>
      lia stato pli kaj pli <tld/>i&gcirc;as.
    </ekz>
    <trdgrp lng="de">
       <trd>schei&szlig;e</trd>,
       <trd>schlecht</trd>,
       <trd>klapprig</trd>,
       <trd>&uuml;bel</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&oacute;cska</trd>,
      <trd>vacak</trd>,
      <trd>sil&aacute;ny</trd>
    </trdgrp>
  </snc>
</subart>
</art>
