    <trd>bodrio</trd>,
    <trd>birria</trd>,
    <trd>chapuza</trd>,
    <trd>cutrez</trd>,
    <trd>chorrada</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cochonnerie</trd>,
    <trd>saloperie</trd>
  </trdgrp>
  <trd lng="he">&dalet;&bet;&resh; &mem;&gimel;&ayin;&yod;&lamed;</trd>
  <trdgrp lng="ja">
