<drv mrk="acx.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="acx.0ajxo.KOMUNE">
    <dif>
      Io tre malpla&ccirc;a, malbona:
      <ekz>
        &scirc;i <klr>[&#x2026;]</klr> ne lasis ian <tld/>a&jcirc;on ie por
        la purigistino
        <fnt>
          <aut>M. Boulton</aut>,
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr33/ebrivirgecou.html">
          Ebrivirgeco</url></vrk>,
          <lok>Nica Literatura Revuo, 1958-01 &gcirc;is 02, numero
          3/3, p.
          99-107a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[jen]</klr> dio ordonas <tld/>a&jcirc;ojn
        <fnt>
          <aut>Zecchin Armando</aut>
          <vrk><url ref="&Monato;/2000/007183.php">Romkatolika</url></vrk>
          <bib>Monato</bib>
          <lok>2000/11, p. 4</lok>
        </fnt>;
      </ekz>
      <ekz>
        meze de la jaro 1475 fama &leftquot;<tld/>a&jcirc;o&#x201D; okazis en la urbo Trento
        <fnt>
          <aut>Armando Zecchin</aut>
          <vrk><url ref="&Monato;/2007/009198.php">&Ccirc;u vere okazis sang-oferaj
          paskoj?</url></vrk>
          <bib>Monato</bib>
          <lok>2007/06, p. 16-17</lok>
        </fnt>;
      </ekz>
      <ekz>
        la uzo de <klr>[&#x2026;]</klr> surogata alfabeto kun la <tld/>a&jcirc;o &leftquot;x&#x201D;
        <fnt>
          <aut>Pejno Simono</aut>
          <vrk><url ref="&Monato;/2011/010412.php">Pri lingvo ne universala, ne el
          Krotelo, ne por Barato kaj nepre sen iksoj</url></vrk>
          <bib>Monato</bib>
          <lok>2011/11, p. 26</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_r;&c_y;&c_d;&c_o;&c_t;&c_a;</trd>,
    <trd>&c_g;&c_ib;&c_d;&c_o;&c_t;&c_a;</trd>
