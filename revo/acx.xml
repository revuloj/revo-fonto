<subart>
  <drv mrk="acx._0">
    <kap>-<tld/></kap>
  <gra><vspec>sufikso</vspec></gra>
  <snc>
    <dif>
      Malbona, malbonkvalita, malta&ubreve;ga, malbela:
      <ekz>
        mizera &ccirc;eval<tld/>o
        <fnt><bib>Fab1</bib>, <lok>Sova&gcirc;aj cignoj</lok></fnt>;
      </ekz>
      <ekz>
        dom<tld/>o <klr>(malbela, kaduka domo)</klr>;
      </ekz>
      <ekz>
        rigard<tld/>i kiel bovido
        <klr>(gapi, stultmiene rigardi)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        babil<tld/>i
        <klr>(stulte, vante a&ubreve; malice paroli)</klr>;
      </ekz>
      <ekz>
        el la far<tld/>o fari&gcirc;is ka&ccirc;o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ie via popol<tld/>o persekutas Fenicianojn, kiel hundoj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi mem faras &ccirc;i tiun simplan laboron kaj ne lasas &gcirc;in kned<tld/>i per iu el viaj
        helpistoj
        <fnt>
          <bib>Fab4</bib>
          <lok>&leftquot;Bela&rightquot;</lok>
        </fnt>!
      </ekz>
      <ekz>
        lia kadavro estos <klr>[&#8230;]</klr> mort<tld/>a&jcirc;o
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i estis mizera, hirta hund<tld/>o, kiu pli similis eluzitan skraptapi&scirc;on
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2006/008788.php">Tenebro</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        fiartefarit<tld/>a&jcirc;o, kiu senkulturigus la homaron!
        <fnt>
          <bib>Monato</bib>
          <aut>Thierry Salomon</aut>
          <vrk><url ref="&Monato;/2003/008016.html">La mondolingvo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fi">fi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Schei&szlig;-<klr> [pref]</klr></trd>,
      <trd>Mist-<klr> [pref]</klr></trd>,
      <trd>Drecks-<klr> [pref]</klr></trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>-aco<klr> (sufijo peyorativo)</klr></trd>,
      <trd>-ucho<klr> (sufijo peyorativo)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>-asse<klr> (suffixe p&eacute;joratif)</klr></trd>,
      <trd>-&acirc;tre<klr> (suffixe p&eacute;joratif)</klr></trd>
    </trdgrp>
    <trd lng="io">-ach</trd>
    <trd lng="vo">lu-</trd>
  </snc>
  <rim>
  Vidu la <ref cel="fi.0.RIM">rimarkon &ccirc;e fi</ref>.
  </rim>
    <trdgrp lng="th">
      <trd>&#3652;&#3617;&#3656;&#3604;&#3637;</trd>,
      <trd>&#3588;&#3640;&#3603;&#3616;&#3634;&#3614;&#3649;&#3618;&#3656;</trd>,
      <trd>&#3627;&#3618;&#3634;&#3610;&#3588;&#3634;&#3618;</trd>
    </trdgrp>
  </drv>
</subart>

