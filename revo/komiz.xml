<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: komiz.xml,v 1.22 2022/03/22 15:44:12 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>komiz</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="komiz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="komiz.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Helpa <ref tip="super" cel="salajr.0ulo">salajrulo</ref>, kiu
      zorgas la vendadon en komercfirmao a&ubreve; la aferojn en banko:
      <ekz>
        la granda, moda kaj ri&ccirc;a magazeno bezonas la servon
        de vera armeo da <tld/>oj, oficistoj k.t.p
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        mi foreksigos miajn <tld/>ojn kaj vin mi petos, ke sur ilian
        lokon
        vi elektu por mi <tld/>inojn el la nombro de viaj protektatinoj
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        <ctl>La malica komizo</ctl>
        <fnt>
          <aut>W. Fern&aacute;ndez-Fl&oacute;rez, trad. F. de Diego</aut>
          <vrk>la malica komizo</vrk>,
          <lok>1993</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="komisi.0ito">komisiito</ref>
    <trdgrp lng="de">
      <trd>Vertreter <klr>(Handel)</klr></trd>,
      <trd>Handelsvertreter</trd>,
      <trd>Vertriebler</trd>,
      <trd>Handelsagent</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>komi</trd>,
    <trd>kom&iacute;</trd>,
    <trd>obchodn&iacute; p&rcaron;&iacute;ru&ccaron;&iacute; <klr>(mu&zcaron;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Handlungsgehilfe</trd>,
    <trd>Verkaufshilfe</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>shop <ind>assistant</ind></trd>,
    <trd>office <ind>assistant</ind></trd>,
    <trd>clerk</trd>,
    <trd>salesman</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>commis<klr tip="ind"> (employ&eacute;)</klr></trd>
    <trd>employ&eacute;<klr tip="ind"> (pr&eacute;pos&eacute; au comptoir d'une
    banque)</klr></trd>,
    <trd>auxiliaire de vente</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kantoorbediende</trd>,
    <trd>winkelbediende</trd>,
    <trd>commis</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ekspedient</trd>,
    <trd>sprzedawca</trd>
  </trdgrp>   
  <trdgrp lng="pt">
    <trd>caixeiro</trd>,
    <trd>balconista</trd>
  </trdgrp>
  <trd lng="ro">v&acirc;nz&abreve;tor</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_k;&c_a;&c_z;&c_ch;&c_i;&c_k;</trd>,
    <trd>&c_t;&c_o;&c_r;&c_g;&c_o;&c_v;&c_y;&c_j; &c_a;&c_g;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_k;&c_l;&c_je;&c_r;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>obchodn&yacute; pomocn&iacute;k <klr>(v obchode, banke)</klr></trd>,
    <trd>obchodn&yacute; pomocn&iacute;k <klr>(v obchode| banke)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x440;&#x438;&#x43A;&#x430;&#x436;&#x447;&#x438;&#x43A; (&#x443; &#x43C;&#x430;&#x433;&#x430;&#x437;&#x438;&#x43D;&#x456;)</trd>,
    <trd>&#x43F;&#x440;&#x43E;&#x434;&#x430;&#x432;&#x435;&#x446;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="komiz.vojagxa0o">
  <kap>voja&gcirc;a <tld/>o, <var><kap>voja&gcirc;anta <tld/>o</kap></var></kap>
  <snc mrk="komiz.vojagxa0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="komerc.0isto">Komercisto</ref>, kiu
      vizitadas
      a&ccirc;etontojn por prezenti specimenojn kaj stimuli a&ccirc;etadon:
      <ekz>
        voja&gcirc;a <tld/>o, revenante hejmen, trovas nekonatan viron
        ku&scirc;anta kun lia edzino
        <fnt>
        <aut>L. Beaucaire</aut>
        <vrk>Kruko kaj Baniko en Bervalo</vrk>
        <lok>Du militas &#x2010; tria profitas</lok>
        </fnt>;
      </ekz>
      <ekz>
        li profesiis kiel voja&gcirc;anta <tld/>o
        <fnt><aut>Pejno S.</aut>:
        <vrk>Misteraj kaj mistifikaj...</vrk>,
        <bib>Monato</bib>, <lok>jaro 1997a, numero 6a, p.
        14a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Handlungsreisender</trd>,
    <trd>Au&szlig;endienstler</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>commis voyageur</trd>,
    <trd>repr&eacute;sentant de commerce</trd>,
    <trd>voyageur de commerce</trd>,
    <trd>V.R.P.</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>handelsreiziger</trd>,
    <trd>handelsvertegenwoordiger</trd>,
    <trd>vertegenwoordiger</trd>
  </trdgrp>
  <trd lng="pl">komiwoja&zdot;er</trd>
  <trdgrp lng="pt">
    <trd>representante comercial itinerante</trd>,
    <trd>caixeiro-viajante</trd>,
    <trd>alabama</trd>,
    <trd>cometa <klr tip="amb">(com.)</klr></trd>
  </trdgrp>
  <trd lng="ro">comis-voiajor</trd>
</drv>

<drv mrk="komiz.migra0o">
  <kap>migra <tld/>o</kap>
  <snc mrk="komiz.migra0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="dif" cel="komiz.vojagxa0o.EKON">Voja&gcirc;a <tld/>o</ref>:
      <ekz>
        du migraj <tld/>oj, kiuj vendas &scirc;ercobjektojn, devas noktumi
        en la sama hotelo <klr>[...]</klr>
        <fnt>
        <aut>L. Beaucaire</aut>
        <vrk>Kruko kaj Baniko en Bervalo</vrk>
        <lok>En fremdan vazon ne &scirc;ovu la nazon</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">obchodn&iacute; cestuj&iacute;c&iacute;</trd>
  <trdgrp lng="de">
    <trd>Handlungsreisender</trd>,
    <trd>Au&szlig;endienstler</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>commis voyageur</trd>,
    <trd>repr&eacute;sentant de commerce</trd>,
    <trd>voyageur de commerce</trd>,
    <trd>V.R.P.</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>handelsreiziger</trd>,
    <trd>handelsvertegenwoordiger</trd>,
    <trd>vertegenwoordiger</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>domokr&aogonek;&zdot;ca</trd>,
    <trd>handlarz obwo&zacute;ny</trd>
  </trdgrp>   
  <trd lng="ro">comer&#x21B; ambulant</trd>
  <trd lng="sk">obchodn&yacute; cestuj&uacute;ci <klr>(nav&scaron;tevuj&uacute;ci
  klientov)</klr></trd>
</drv>
</art>
