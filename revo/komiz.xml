<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>komiz</rad>/o
</kap>
<drv mrk="komiz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Helpa salajrulo, kiu zorgas la vendadon en komercfirmo a&ubreve;
      la aferojn en banko.  
    </dif>
  </snc>
  <trd lng="de">Handlungsgehilfe</trd>
  <trdgrp lng="en">
    <trd>shop <ind>assistant</ind></trd>,
    <trd>office <ind>assistant</ind></trd>,
    <trd>clerk</trd>,
    <trd>salesman</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kantoorbediende</trd>,
    <trd>winkelbediende</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_k;&c_a;&c_z;&c_ch;&c_i;&c_k;</trd>,
    <trd>&c_t;&c_o;&c_r;&c_g;&c_o;&c_v;&c_y;&c_j; &c_a;&c_g;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_k;&c_l;&c_je;&c_r;&c_k;</trd>
  </trdgrp>
</drv>
</art>
