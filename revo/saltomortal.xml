<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>saltomortal</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="saltomortal.0o">
  <kap><tld/>o</kap>
  <snc mrk="saltomortal.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="super" cel="salt.0o.KOMUNE">Salto</ref> anta&ubreve;en kun
      enaera
      renverso de la tuta korpo kaj restari&gcirc;o:
      <ekz>
        La delfenoj <klr>...</klr> dum la tago gaje saltas &ccirc;irka&ubreve; la
        &scirc;ipo
        kaj iafoje faras verajn <tld/>ojn
        <fnt><bib>Voja&gcirc;Imp</bib>, <lok>el odeso &gcirc;is
        Konstantinopolo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">salto mortal</trd>
  <trd lng="fr">saut p&eacute;rilleux</trd>
  <trdgrp lng="hu">
    <trd>hal&aacute;lugr&aacute;s</trd>,
    <trd>szalt&oacute;</trd>,
    <trd>szalt&oacute; mort&aacute;le</trd>
  </trdgrp>
  <trd lng="it"><ind>salto</ind> mortale</trd>
  <trd lng="pl">salto mortale</trd>
  <trdgrp lng="pt">
   <trd>salto mortal</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_a;&c_l;&c_mol;&c_t;&c_o;-&c_m;&c_o;&c_r;&c_t;&c_a;&c_l;&c_je;</trd>
</drv>
</art>
