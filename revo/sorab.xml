<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>sorab</rad>/o 
</kap>
<drv mrk="sorab.0o">
  <kap><tld/>o</kap>
  <snc mrk="sorab.0o.popolano">
    <uzo tip="fak"></uzo>
    <dif>
      Ano de malgranda okcidentslava 
      <ref tip="super" cel="popol.0o.etno">popolo</ref>, &scirc;tate agnoskita
      
      etna minoritato, vivanta en orienta 
      <ref tip="malprt" cel="german1.G0ujo">Germanujo</ref>:
      <ekz>
        Heinrich I. submetas la sorabajn tribojn de Luzicoj kaj Milcenoj
        <fnt><vrk><url>http://www.sorbzilla.de/soraboj/sorabojengermanio/Soraboj7.htm</url></vrk></fnt>
      </ekz>
    </dif>
    <ref tip="lst" cel="gent.0o.raso" lst="voko:gentoj">gento</ref>
  </snc>
  <trd lng="de">Sorbe</trd>
</drv>
</art>

