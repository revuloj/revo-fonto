<art mrk="$Id: formin.xml,v 1.16 2023/11/21 15:28:32 revo Exp $">
<kap>
<rad>forming</rad>/o
</kap>
<drv mrk="formin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Antikva sentu&scirc;ila <ref tip="lst" cel="instru2.kord0o.MUZ"
      lst="voko:kordinstrumentoj">kordinstrumento</ref>,
      komuna &ccirc;e helenoj en la unua duono de la unua jarmilo a.K.:
      <ekz>
        la <tld/>o poste estis anstata&ubreve;igata de <ref cel="citar.0o">kitaro</ref>
        kaj <ref cel="lir.0o">liro</ref>;
      </ekz>
      <ekz>
        Homero priskribis la <tld/>on en la Iliado kaj la Odiseado,
        &gcirc;i akompanis la <ref cel="rapsod.0o">rapsodo</ref>jn.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/e/e8/939b2.jpg/282px-939b2.jpg" prm="CC-BY-SA-3.0"><tld/>o
        <fnt>
          <aut>Yolanda Chalvatzi, el magazino de Universitato de
          Pireo</aut>
          <lok><url ref="&WCW;/File:939b2.jpg">&WCW;/File:939b2.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_f;&c_o;&c_r;&c_m;&c_ib;&c_n;&c_g;&c_a; <klr>(&c_m;&c_u;&c_z;. &c_ib;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_e;&c_n;&c_t;)</klr></trd>
  <trd lng="cs">forminx</trd>
  <trd lng="de">Phorminx</trd>
  <trd lng="fr">phorminx</trd>
  <trd lng="hu">forminx</trd>
  <trd lng="ja">&#x30D5;&#x30A9;&#x30EB;&#x30DF;&#x30F3;&#x30AF;&#x30B9;</trd>
  <trd lng="pt">forminge</trd>
  <trd lng="sk">forminx</trd>
  <trd lng="uk">&c_f;&#x43E;&c_r;&#x43C;&c_ib;&#x43D;&#x43A;&c_s;</trd>
</drv>
</art>
