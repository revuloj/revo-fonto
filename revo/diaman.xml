<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: diaman.xml,v 1.30 2023/11/04 14:29:01 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>diamant</rad>/o <fnt><bib>VdE</bib></fnt>
</kap>

<drv mrk="diaman.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="diaman.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      <ref tip="super" cel="sxton.juvel0o">Juvel&scirc;tono</ref>, el pura kristali&gcirc;inta
      <ref tip="super" cel="karbon.0o">karbono</ref>:
      <ekz>
        la <tld/>o havas belan brilon
        <fnt>
          <bib>F</bib>
          <lok>&FE; 40</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iaspecaj multekostaj &scirc;tonoj vin kovris: rubeno, topazo, diamanto, <klr>[&#x2026;]</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 28:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ajnas al mi, ke el &scirc;ia bu&scirc;o elsaltas perloj kaj <tld/>oj
        <fnt>
          <bib>F</bib>
          <lok>&FE; 17</lok>
        </fnt>!
      </ekz>
      <ekz>
        la kolo de la botelo restis nerompita, kaj oni &gcirc;in detran&ccirc;is per <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malbono strekas la malsa&gcirc;ulon
        kiel la <tld/>o strekas malmolan gemon
        <fnt>
          <aut>trad. T. T. Anuruddha</aut>,
          <vrk>La Vojo al Nirvano a&ubreve; la Dhammapado, 12:161</vrk>,
          <lok>B&#x1ED3;-T&aacute;t-Csoma-Instituto por Budhologio, 1973</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne &ccirc;io brilanta estas <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="brilia.0o.MIN">brilianto</ref>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_m;&c_e;&c_n;&c_t;</trd>
  <trd lng="bo">&t_ra;&t_data;&t_naro;&t_tsheg;&t_ra;&t_jata;&t_drengbu;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>diamant</trd>,
    <trd>d&eacute;mant</trd>
  </trdgrp>
  <trd lng="de">Diamant</trd>
  <trd lng="en">diamond</trd>
  <trd lng="es">diamante</trd>
  <trd lng="fa">&a_A;&a_l;&a_m;&a_A;&a_s;</trd>
  <trd lng="fr">diamant</trd>
  <trd lng="he">&yod;&he;&lamed;&vav;&fmem;</trd>
  <trd lng="hu">gy&eacute;m&aacute;nt</trd>
  <trd lng="ja">&#x30C0;&#x30A4;&#x30E4;&#x30E2;&#x30F3;&#x30C9;</trd>
  <trd lng="nl">diamant</trd>
  <trd lng="pl">diament</trd>
  <trd lng="pt">diamante</trd>
  <trd lng="ru">&c_a;&c_l;&c_m;&c_a;&c_z;</trd>
  <trd lng="sk">diamant</trd>
  <trd lng="sv">diamant</trd>
  <trdgrp lng="uk">
    <trd>&c_a;&c_l;&c_m;&c_a;&c_z;</trd>,
    <trd>&c_d;&c_ib;&c_a;&c_m;&c_a;&c_n;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x91D1;&#x521A;&#x77F3; <pr>j&#x12B;ng&#x101;ngsh&#xED;</pr></trd>,
    <trd>&#x91D1;&#x521A; <pr>j&#x12B;ng&#x101;ng</pr></trd>,
    <trd>&#x947D; <pr>zu&#x101;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="diaman.0a">
  <kap><tld/>a</kap>
  <snc mrk="diaman.0a.MIN">
    <dif>
      El <tld/>o:
      <ekz>
        mi prenis <tld/>an ringon de mia malgranda fingro kaj metis &gcirc;in sur &scirc;ian manon
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        skribita per <klr>[&#x2026;]</klr> per <tld/>a pinto
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 17:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili skribadis sur oraj tabeloj per <tld/>aj grifeloj
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a kol&ccirc;eno
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LVI</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">de <ind>diamant</ind></trd>
    <trd lng="ru">&c_a;&c_l;&c_m;&c_a;&c_z;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="diaman.0a.FIG">
    <dif>
      <ref tip="dif" cel="brilia.0a">Brilianta</ref>:
      <ekz>
        <tld/>a a&ubreve;reolo
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        matenon ni havis <tld/>an teon, tio estis la roso
        <fnt>
          <bib>Fab1</bib>
          <lok>Fluganta kofro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="diaman.0a.jubilea">
    <uzo tip="fak">KAL</uzo>
    <dif>
      <klr>(jubileo)</klr> 75-jara:
      <ekz>
        <tld/>a geedzi&gcirc;o;
      </ekz>
      <ekz>
        <klr>[okazas]</klr> la tiel nomata <tld/>a jubileo de la re&gcirc;ino Elizabeta 2a,
        kiu anta&ubreve; 60 jaroj surtroni&gcirc;is
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2012/010675.php">La&ubreve;du lernejanojn!</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_m;&c_e;&c_n;&c_t;&c_a;&c_v;&c_y;</trd>
  <trd lng="de">diamanten</trd>
  <trd lng="en">diamond</trd>
  <trd lng="fa">&a_A;&a_l;&a_m;&a_A;&a_s;&f_g;&a_w;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>adamantin</trd>,
    <trd>diamantin</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gy&eacute;m&aacute;nt-</trd>,
    <trd>gy&eacute;m&aacute;ntos</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30C0;&#x30A4;&#x30E4;&#x30E2;&#x30F3;&#x30C9;&#x306E;  <pr>&#x3060;&#x3044;&#x3084;&#x3082;&#x3093;&#x3069;&#x306E;</pr></trd>,
    <trd>&#x30C0;&#x30A4;&#x30E4;&#x306E;&#x3088;&#x3046;&#x306B;&#x304D;&#x3089;&#x3081;&#x304F;  <pr>&#x3060;&#x3044;&#x3084;&#x306E;&#x3088;&#x3046;&#x306B;&#x304D;&#x3089;&#x3081;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">diamanten<klr tip="ind"> (bijv.nw.)</klr></trd>
  <trd lng="pl">diamentowy</trd>
  <trd lng="sk">diamantov&yacute;</trd>
  <trd lng="zh">&#x91D1;&#x521A;&#x77F3;&#x5236;&#x7684; <pr>j&#x12B;ng&#x101;ngsh&#xED;zh&#xEC;de</pr></trd>
</drv>
</art>
