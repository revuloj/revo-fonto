<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>diamant</rad>/o
</kap>
<drv mrk="diaman.0o">
<kap><ofc>*</ofc><tld/>o</kap>
<uzo tip="fak">MIN</uzo>
<snc>
<dif>
Juvel&scirc;tono, el pura kristali&gcirc;inta karbono.
</dif>
<refgrp tip="sub">
<ref cel="brilia.0o">brilianto</ref>.
</refgrp>
</snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_m;&c_e;&c_n;&c_t;</trd>
  <trd lng="de">Diamant</trd>
  <trd lng="es">diamante</trd>
  <trd lng="fr">diamant</trd>
  <trd lng="hu">gy&eacute;m&aacute;nt</trd>
  <trd lng="nl">diamant</trd>
  <trd lng="sv">diamant</trd>
  <trd lng="ru">&c_a;&c_l;&c_m;&c_a;&c_z;</trd>
  <trd lng="sk">diamant</trd>
</drv>
<drv mrk="diaman.0a">
<kap><tld/>a</kap>
<snc num="1">
<dif>
El <tld/>o. 
</dif>
<trd lng="de">diamanten</trd>
<trd lng="fr">de <ind>diamant</ind></trd>
</snc>
<snc num="2">
<dif>
Brila, kiel <tld/>o: <ekz><tld/>aj rosaj gutoj.
</ekz>
</dif>
</snc>
<trd lng="be">&c_d;&c_y;&c_ja;&c_m;&c_e;&c_n;&c_t;&c_a;&c_v;&c_y;</trd>
<trdgrp lng="fr">
  <trd>diamantin</trd>,
  <trd>adamantin</trd></trdgrp> 
<trdgrp lng="hu">
  <trd>gy&eacute;m&aacute;nt-</trd>,
  <trd>gy&eacute;m&aacute;ntos</trd></trdgrp>
<trd lng="nl">diamanten<klr tip="ind"> (bijv.nw.)</klr></trd>
<trd lng="ru">&c_a;&c_l;&c_m;&c_a;&c_z;&c_n;&c_y;&c_j;</trd>
<trd lng="sk">diamantov&yacute;</trd>
</drv>
</art>
