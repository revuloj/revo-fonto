<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>zo</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="zo.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="zo.0o.bestogxardeno">
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="gxarde.besto0o.TUR">bestoĝardeno</ref>
    <ekz>
      kial leono povis forkuri de sia kaĝo, kaj transsalti la altajn
      murojn de la <tld/>o
      <fnt>
      <aut>Geraldo Mattos</aut>,
      <vrk><url
      ref="http://www.geocities.com/EnchantedForest/2755/mat04l3.html">La
      amika malamiko</url></vrk>,
      <lok>1998</lok>
      </fnt>?
    </ekz>
    <ekz>
      <tld/>o Zoorasia <klr>...</klr> estas la plej vasta <tld/>o en
      Jokohamo
      <fnt>
      <aut>-</aut>,
      <vrk><url
 
ref="http://www.k4.dion.ne.jp/~uk2007/espe/vidinda/index.html">Gvidlibro
      pri Jokohamo</url></vrk>,
      <lok>10. Norda parto..., 2005</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="fr">
    <trd>jardin zoologique</trd>,
    <trd>zoo</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ogród zoologiczny</trd>,
    <trd>ZOO</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>jardim zoológico</trd>,
    <trd>zoológico</trd>,
    <trd>zôo</trd>
  </trdgrp>
</drv>
</art>

