<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>zo</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="zo.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="zo.0o.bestogxardeno">
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="gxarde.besto0o.TUR">besto&gcirc;ardeno</ref>
    <ekz>
      kial leono povis forkuri de sia ka&gcirc;o, kaj transsalti la altajn
      murojn de la <tld/>o
      <fnt>
      <aut>G. Mattos</aut>,
      <vrk>
<url ref="http://www.geocities.com/EnchantedForest/2755/mat04l3.html">
      La amika malamiko</url></vrk>,
      <lok>1998</lok>
      </fnt>?
    </ekz>
    <ekz>
      <tld/>o Zoorasia <klr>...</klr> estas la plej vasta <tld/>o en
      Jokohamo
      <fnt>
        <aut>J. Aida, K. Suzuki, M. &Scirc;idara</aut>,
        <vrk>
<url ref="http://www.k4.dion.ne.jp/~uk2007/espe/vidinda/index.html">
        Gvidlibro pri Jokohamo</url></vrk>,
        <lok>10. Norda parto..., 2005</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="bg">&c_z;&c_o;&c_o;&c_p;&c_a;&c_r;&c_k;</trd>
  <trdgrp lng="de">
    <trd>zoologischer Garten</trd>,
    <trd>Zoo</trd>
  </trdgrp>
  <trd lng="fa">&a_b;&a_A;&a_gh;&zwnj;&a_w;&a_H;&a_sh;</trd>
  <trdgrp lng="fr">
    <trd>jardin zoologique</trd>,
    <trd>zoo</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ogr&oacute;d zoologiczny</trd>,
    <trd>ZOO</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>jardim zool&oacute;gico</trd>,
    <trd>zool&oacute;gico</trd>,
    <trd>z&ocirc;o</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>giardino zoologico</trd>,
    <trd>zoo</trd>
  </trdgrp>
  <trd lng="tr">hayvanat bah&ccedil;esi</trd>
</drv>
</art>
