<vortaro>
<art mrk="$Id: zo.xml,v 1.19 2020/04/25 15:10:33 revo Exp $">
<kap>
  <rad>zo</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="zo.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="zo.0o.bestogxardeno">
    <uzo tip="fak">ZOO</uzo>
    <dif>
       <ref tip="dif" cel="gxarde.besto0o.TUR">Besto&gcirc;ardeno</ref>:
      <ekz>
        kial leono povis forkuri de sia ka&gcirc;o, kaj transsalti la altajn
        murojn de la <tld/>o
        <fnt>
        <aut>G. Mattos</aut>,
        <vrk><url
        ref="http://www.geocities.com/EnchantedForest/2755/mat04l3.html">
        La amika malamiko</url></vrk>,
        <lok>1998</lok>
        </fnt>?
      </ekz>
      <ekz>
        <nom>Zoorasia</nom> <klr>[&#8230;]</klr> estas la plej vasta <tld/>o
        en
        Jokohamo
        <fnt>
          <aut>J. Aida, K. Suzuki, M. &Scirc;idara</aut>,
          <vrk><url
          ref="http://www.k4.dion.ne.jp/~uk2007/espe/vidinda/index.html">
          Gvidlibro pri Jokohamo</url></vrk>,
          <lok>10. Norda parto..., 2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="zoolog1.0a_gxardeno">zoologia &gcirc;ardeno</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_a;&c_p;&c_a;&c_r;&c_k;</trd>,
    <trd>&c_z;&c_a;&c_a;&c_s;&c_a;&c_d;</trd>
  </trdgrp>
  <trd lng="bg">&c_z;&c_o;&c_o;&c_p;&c_a;&c_r;&c_k;</trd>
  <trd lng="ca">zoo</trd>
  <trdgrp lng="cs">
    <trd>zoo</trd>,
    <trd>zoologick&aacute; zahrada</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>zoologischer Garten</trd>,
    <trd>Zoo</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>jard&iacute;n zool&oacute;gico</trd>,
    <trd>zool&oacute;gico</trd>,
    <trd>zoo</trd>
  </trdgrp>
  <trd lng="fa">&a_b;&a_A;&a_gh;&zwnj;&a_w;&a_H;&a_sh;</trd>
  <trdgrp lng="fr">
    <trd>jardin zoologique</trd>,
    <trd>zoo</trd>
  </trdgrp>
  <trd lng="id"><ind>kebun</ind> binatang</trd>
  <trdgrp lng="it">
    <trd>giardino zoologico</trd>,
    <trd>zoo</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ogr&oacute;d zoologiczny</trd>,
    <trd>ZOO</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>jardim zool&oacute;gico</trd>,
    <trd>zool&oacute;gico</trd>,
    <trd>z&ocirc;o</trd>
  </trdgrp>
  <trd lng="sk">zoologick&aacute; z&aacute;hrada</trd>
  <trd lng="tr">hayvanat bah&ccedil;esi</trd>
</drv>
</art>
