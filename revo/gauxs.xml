<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Ga&ubreve;s</rad>/o
</kap>
<drv mrk="gauxs.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <dif>
      Germanlingve: Karl Friedrich <trd lng="de">Gau&szlig;</trd>, 1777-1855.
      Germana matematikisto, astronomo kaj fizikisto.
    </dif>
  </snc>
  <trd lng="en">Gauss</trd>
  <trd lng="fr">Gauss</trd>
  <trd lng="ru">&c_G;&c_a;&c_u;&c_s;&c_s;</trd>
</drv>

<drv mrk="gauxs.g0a">
  <kap><tld lit="g"/>a</kap>
  <fnt><bib>PIV1</bib><lok>artikolo: ga&ubreve;so</lok></fnt>
  <snc>
    <refgrp tip="vid">
      <ref cel="entjer.gauxsa0o"><tld lit="g"/>a entjero</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Gau&szlig;sche</trd>
  <trdgrp lng="en">
    <trd>Gauss['s]</trd>,
    <trd>Gaussian</trd></trdgrp> 
  <trdgrp lng="fr">
    <trd>de <ind>Gauss</ind></trd>,
    <trd>gaussien</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_G;&c_a;&c_u;&c_s;&c_s;&c_a;</trd>,
    <trd>&c_g;&c_a;&c_u;&c_s;&c_s;&c_o;&c_v;</trd></trdgrp> 
</drv>
<adm>
  Por la fizika mezurunuo "gauxso" uzu la ekzemplon de "ampero".
  <aut>MB</aut>
</adm>
</art>

<!--
$Log$
-->
</vortaro>