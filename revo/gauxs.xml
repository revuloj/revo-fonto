<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gauxs.xml,v 1.14 2023/11/11 10:00:39 revo Exp $">
<kap>
   <rad>Ga&ubreve;s</rad>/o
</kap>
<drv mrk="gauxs.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">PERS</uzo>
   <fnt><bib>PIV2</bib></fnt>
  <snc>
    <dif>
      Germanlingve: <nom>Karl Friedrich</nom> <trd lng="de">Gau&szlig;</trd>,
      1777-1855,
      germana <ref tip="lst" lst="voko:historiaj_personoj" cel="matema.0isto">matematikisto</ref>,
      astronomo kaj fizikisto:
      <ekz>
        en 1831, rekomendita de sia amiko <tld/>o, <klr>[<nom>Wilhelm
        Eduard Weber</nom>]</klr>
        estis nomita al <nom>G&ouml;ttingen</nom> (kie <tld/>o estis
        direktoro de la astronomia
        observatorio) kiel instruisto pri fiziko, kiam li estis 27
        jarojn a&gcirc;a
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=414564">Wilhelm Eduard Weber</url></lok>
        </fnt>;
      </ekz>
    </dif>
  </snc>
   <trd lng="be">&c_G;&c_a;&c_w;&c_s;</trd>
   <trd lng="ca">Gauss</trd>
   <trd lng="en">Gauss</trd>
   <trd lng="es">Gauss</trd>
   <trd lng="fi">Gauss</trd>
   <trd lng="fr">Gauss</trd>
   <trd lng="hu">Gauss</trd>
   <trd lng="pl">Gauss</trd>
   <trd lng="pt">Gauss</trd>
   <trd lng="ru">&c_G;&c_a;&c_u;&c_s;&c_s;</trd>
  <trd lng="zh">&#x78C1;&#x529B;&#x5355;&#x4F4D; <pr>c&#xED;l&#xEC;d&#x101;nw&#xE8;i</pr></trd>
</drv>

<drv mrk="gauxs.g0a">
   <kap><tld lit="g"/>a</kap>
   <fnt><bib>PIV1</bib><lok>artikolo: ga&ubreve;so</lok></fnt>
   <snc>
     <refgrp tip="vid">
       <ref cel="entjer.gauxsa0o"><tld lit="g"/>a entjero</ref>.
     </refgrp>
   </snc>
   <trdgrp lng="be">
     <trd>&c_g;&c_a;&c_w;&c_s;&c_a;&c_w;</trd>,
     <trd>&c_G;&c_a;&c_w;&c_s;&c_a;</trd>
   </trdgrp>
   <trd lng="ca">gaussi&agrave;</trd>
   <trd lng="de">Gau&szlig;sche</trd>
   <trdgrp lng="en">
     <trd>Gauss['s]</trd>,
     <trd>Gaussian</trd></trdgrp>
   <trd lng="es">gaussiano</trd>
   <trd lng="fi">Gaussin</trd>
   <trdgrp lng="fr">
     <trd>de <ind>Gauss</ind></trd>,
     <trd>gaussien</trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>Gauss-f&eacute;le</trd>,
     <trd>gaussi</trd></trdgrp>
   <trd lng="pl">Gaussa</trd>
   <trd lng="pt">gaussiano</trd>
   <trdgrp lng="ru">
     <trd>&c_G;&c_a;&c_u;&c_s;&c_s;&c_a;</trd>,
     <trd>&c_g;&c_a;&c_u;&c_s;&c_s;&c_o;&c_v;</trd></trdgrp>
</drv>

<drv mrk="gauxs.g0o">
  <kap><tld lit="g"/>o</kap>
  <snc mrk="gauxs.0o.unuo">
   <mlg kod="CGS">Gs</mlg>
    <dif>
      <ref tip="lst" lst="voko:mezurunuoj" cel="unu.mezur0o.SCI">Mezurunuo</ref> de magneta
      induktdenso en la sistemo CGS:
      <ekz>
        la <tld lit="g"/>o <klr>[&#x2026;]</klr> oma&gcirc;as la germanan
        matematikiston kaj
        fizikiston <nom>Carl Friedrich Gauss</nom>, 1 <tld lit="g"/>o &#x2261;
        1 Mx/cm2, <klr>[&#x2026;]</klr>
        ekzemple la tera magneta kampo valoras &ccirc;irka&ubreve; 0,5 G
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=318351">Ga&ubreve;so (mezurunuo)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      La sistemo SI uzas la mezurunon Teslo (T).
    </rim>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_w;&c_s; <klr>(&c_a;&c_d;&c_z;&c_ib;&c_n;&c_k;&c_a; &c_v;&c_y;&c_m;&c_ja;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>
  <trd lng="uk">&c_Gu;&c_a;&c_u;&c_s;</trd>
</drv>

<adm>
   Por la fizika mezurunuo "gauxso" uzu la ekzemplon de "ampero".
   <aut>MB</aut>
</adm>
</art>

