<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: merlan.xml,v 1.22 2024/11/04 01:58:37 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>merlang</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="merlan.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="merlan.0o.marfisxo">
    <dif>
      <ref tip="lst" cel="fisx.mar0o" lst="voko:fi&scirc;oj">Marfi&scirc;o</ref>,
      <ref tip="super" cel="gad.0o.KOMUNE">gado</ref> kun
      malhelaj makuletoj &ccirc;e la brustna&gcirc;iloj;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref>
      el familio <ref tip="malprt" cel="gaded.0oj">gadedoj</ref>
      (<trd lng="la">Merlangius</trd>):
      <ekz>
        <tld/>o estas salakva rabofi&scirc;o <klr>[&#8230;]</klr>, la maksimuma longo estas proksimume 70
centimetroj,
        oni multe kaptas &gcirc;in por man&gcirc;i
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=140249">Merlango</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/2a/Wijting002.jpg/320px-Wijting002.jpg" prm="CC BY 3.0"><tld/>oj
        <fnt>
          <aut>Georges Jansoone (JoJan), propra verko</aut>
          <lok><url ref="&WCW;/File:Wijting002.jpg">&WCW;/File:Wijting002.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_r;&c_l;&c_a;&c_n;</trd>
  <trdgrp lng="de">
    <trd>Merlan</trd>,
    <trd>Wei&szlig;ling</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>whiting</trd>,
    <trd>merling</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>merl&aacute;n</trd>,
    <trd>plegonero</trd>
  </trdgrp>
  <trd lng="fr">merlan</trd>
  <trd lng="hu">v&eacute;konybajsz&uacute; <ind>t&odblac;kehal</ind></trd>
  <trdgrp lng="it">
    <trd>merlano</trd>,
    <trd>nasello</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12507;&#12527;&#12452;&#12486;&#12451;&#12531;&#12464;</trd>,
    <trd>&#12513;&#12523;&#12521;&#12531;</trd>
  </trdgrp>
  <trd lng="nl">wijting</trd>
  <trd lng="pl">witlinek</trd>
  <trd lng="pt">pescada <klr tip="amb">(ictiol.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_r;&c_l;&c_a;&c_n;</trd>,
    <trd>&c_m;&c_je;&c_r;&c_l;&c_a;&c_n;&c_g;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_m;&c_je;&c_r;&c_l;&c_a;&c_n;</trd>,
    <trd>&c_m;&c_je;&c_r;&c_l;&c_a;&c_n;&c_g;</trd>,
    <trd>&c_ch;&c_o;&c_r;&c_n;&c_o;&c_m;&c_o;&c_r;&c_s;&c_mol;&c_k;&c_a; &c_p;&c_ib;&c_k;&c_sh;&c_a;</trd>
  </trdgrp>
</drv>
</art>
