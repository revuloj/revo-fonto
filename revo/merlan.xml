<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>merlang</rad>/o
</kap>
<drv mrk="merlan.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="gaded.0oj">gadedoj</ref>
      (<trd lng="la">Merlangus</trd>), vivanta en salakvo kaj kun malhelaj
      makuletoj &ccirc;e la brustna&gcirc;iloj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Merlan</trd>,
    <trd>Wei&szlig;ling</trd>
  </trdgrp>
  <trd lng="en">whiting</trd>
  <trdgrp lng="es">
    <trd>merl&aacute;n</trd>,
    <trd>plegonero</trd>
  </trdgrp>
  <trd lng="fr">merlan</trd>
  <trdgrp lng="it">
    <trd>merlano</trd>,
    <trd>nasello</trd>
  </trdgrp>
  <trd lng="nl">wijting</trd>
  <trd lng="pt">badejo</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_r;&c_l;&c_a;&c_n;</trd>,
    <trd>&c_m;&c_je;&c_r;&c_l;&c_a;&c_n;&c_g;</trd>
  </trdgrp>
</drv>
</art>
