<art mrk="$Id: merlan.xml,v 1.19 2023/09/02 06:04:11 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>merlang</rad>/o
</kap>
<drv mrk="merlan.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="fisx.mar0o" lst="voko:fi&scirc;oj">Marfi&scirc;o</ref>,
      <ref tip="super" cel="gad.0o.KOMUNE">gado</ref> kun
      malhelaj makuletoj &ccirc;e la brustna&gcirc;iloj;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio <ref tip="malprt" cel="gaded.0oj">gadedoj</ref>
      (<trd lng="la">Merlangius</trd>):
      <ekz>
        <tld/>o estas salakva rabofi&scirc;o <klr>[&#x2026;]</klr>, la maksimuma longo estas proksimume 70&nbsp;centimetroj,
        oni multe kaptas &gcirc;in por man&gcirc;i
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=140249">Merlango</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/2a/Wijting002.jpg/320px-Wijting002.jpg" prm="CC BY 3.0"><tld/>oj
        <fnt>
          <aut>Georges Jansoone (JoJan), propra verko</aut>
          <lok><url ref="&WCW;/File:Wijting002.jpg">&WCW;/File:Wijting002.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="de">
    <trd>Merlan</trd>,
    <trd>Wei&szlig;ling</trd>
  </trdgrp>
  <trd lng="en">whiting</trd>
  <trdgrp lng="es">
    <trd>merl&aacute;n</trd>,
    <trd>plegonero</trd>
  </trdgrp>
  <trd lng="fr">merlan</trd>
  <trd lng="hu">v&eacute;konybajsz&uacute; <ind>t&odblac;kehal</ind></trd>
  <trdgrp lng="it">
    <trd>merlano</trd>,
    <trd>nasello</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30E1;&#x30EB;&#x30E9;&#x30F3;</trd>,
    <trd>&#x30DB;&#x30EF;&#x30A4;&#x30C6;&#x30A3;&#x30F3;&#x30B0;</trd>
  </trdgrp>
  <trd lng="nl">wijting</trd>
  <trd lng="pl">witlinek</trd>
  <trd lng="pt">pescada <klr tip="amb">(ictiol.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_r;&c_l;&c_a;&c_n;</trd>,
    <trd>&c_m;&c_je;&c_r;&c_l;&c_a;&c_n;&c_g;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x435;&#x440;&#x43B;&#x430;&#x43D;</trd>,
    <trd>&#x43C;&#x435;&#x440;&#x43B;&#x430;&#x43D;&#x433;</trd>,
    <trd>&#x447;&#x43E;&#x440;&#x43D;&#x43E;&#x43C;&#x43E;&#x440;&#x441;&#x44C;&#x43A;&#x430; &#x43F;&#x456;&#x43A;&#x448;&#x430;</trd>
  </trdgrp>
</drv>
</art>
