<art mrk="$Id: ponard.xml,v 1.29 2024/02/17 07:52:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ponard</rad>/o
</kap>
<drv mrk="ponard.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Pinta amba&ubreve;tran&ccirc;a mallonga pik<ref tip="lst" lst="voko:bataliloj" cel="arm.0ilo">armilo</ref>:
      <ekz>
        eliros la lo&gcirc;antoj de la urboj de <nom>Izrael</nom>, faros fajron, kaj bruligos la
        armilojn, la &scirc;ildojn kaj kirasojn, la pafarkojn kaj sagojn, la <tld/>ojn kaj lancojn
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 39:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        ludante, l' araboj <tld/>ojn &jcirc;etadis kaj ilin en flugo kun &gcirc;ojo kaptadis
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la unua vorto, kiu eliros el viaj lipoj, frapos kiel mortiga <tld/>o la koron de viaj fratoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Greko eltiris maldikan <tld/>on kaj apogis &gcirc;in sur &scirc;ia nuko
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Herhor</nom> eltiris el sub sia blanka vesto <tld/>on kaj
        faris movon, kvaza&ubreve; volante frapi Likonon inter la okulojn
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi estas virino respondis la figuro per tiel penetrema vo&ccirc;o, ke &gcirc;i trapikis lian koron, kiel
        fenicia <tld/>o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IX</lok>
       ponardo </fnt>;
      </ekz>
      <ekz>
        morti &dash; dormi &dash; trankvile dormi! &dash; per unu pu&scirc;o de <tld/>o, li povus sin de &ccirc;io liberigi
        <fnt>
          <aut>L. L. Zamenhof</aut>
          <vrk>Hamleto, el Hamleto</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kvaza&ubreve; miloj da <tld/>oj, kiuj traflugadis la cerbon, pikadis la tempiojn kaj faladis per
        siaj pintoj sur la fundon de la brusto
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_n;&c_zh;&c_a;&c_l;</trd>
  <trd lng="bg">&c_k;&c_a;&c_m;&c_a;</trd>
  <trd lng="cs">d&yacute;ka</trd>
  <trd lng="de">Dolch</trd>
  <trd lng="en">dagger</trd>
  <trd lng="es">daga</trd>
  <trd lng="fa">&a_kh;&a_n;&a_j;&a_r;</trd>
  <trd lng="fr">poignard</trd>
  <trd lng="hu">t&odblac;r</trd>
  <trd lng="ja">&#x77ED;&#x5263;  <pr>&#x305F;&#x3093;&#x3051;&#x3093;</pr></trd>
  <trd lng="nl">dolk</trd>
  <trd lng="pt">punhal</trd>
  <trd lng="ru">&c_k;&c_i;&c_n;&c_zh;&c_a;&c_l;</trd>
  <trd lng="sk">d&yacute;ka</trd>
  <trd lng="sv">dolk</trd>
  <trd lng="uk">&#x43A;&#x438;&#x43D;&#x434;&#x436;&#x430;&#x43B;</trd>
  <trdgrp lng="zh">
    <trd>&#x5C16;&#x5200; <pr>ji&#x101;nd&#x101;o</pr></trd>,
    <trd>&#x5315;&#x9996; <pr>b&#x1D0;sh&#x1D2;u</pr></trd>,
    <trd>&#x5315; <pr>b&#x1D0;</pr></trd>,
    <trd>&#x77ED;&#x5251; <pr>du&#x1CE;nji&#xE0;n</pr></trd>,
    <trd>&#x77ED;&#x528D; <pr>du&#x1CE;nji&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ponard.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="pik.0i.enigi_pinton">Piki</ref>
      per <tld/>o:
      <ekz>
        kadavroj &ndash; mortpafitaj, venenitaj, <tld/>itaj &ndash; sterni&gcirc;as sur tapi&scirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2012/010647.php">Krimoj senviktimaj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_d;&c_a;&c_r;&c_y;&c_c;&c_mol; <ind>&c_k;&c_ib;&c_n;&c_zh;&c_a;&c_l;&c_a;&c_m;</ind></trd>
  <trdgrp lng="bg">
    <trd>&c_p;&c_r;&c_o;&c_m;&c_u;&c_sh;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_r;&c_malmol;&c_g;&c_v;&c_a;&c_m; &c_s; <ind>&c_k;&c_a;&c_m;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="cs">bodnout d&yacute;kou</trd>
  <trd lng="de">erdolchen</trd>
  <trd lng="en">stab</trd>
  <trd lng="es">apu&ntilde;alar</trd>
  <trd lng="fa">&a_kh;&a_n;&a_j;&a_r; &a_z;&a_d;&a_n;</trd>
  <trd lng="fr">poignarder</trd>
  <trd lng="hu">t&odblac;rrel lesz&uacute;r</trd>
  <trd lng="ja">&#x77ED;&#x5263;&#x3067;&#x523A;&#x3059;  <pr>&#x305F;&#x3093;&#x3051;&#x3093;&#x3067;&#x3055;&#x3059;</pr></trd>
  <trd lng="nl">met een <ind>dolk</ind> steken</trd>
  <trd lng="ru">&c_u;&c_d;&c_a;&c_r;&c_i;&c_t;&c_mol; <ind>&c_k;&c_i;&c_n;&c_zh;&c_a;&c_l;&c_o;&c_m;</ind></trd>
  <trd lng="sk">bodn&uacute;&tcaron; d&yacute;kou</trd>
  <trd lng="uk">&#x443;&#x434;&#x430;&#x440;&#x44F;&#x442;&#x438; &#x43A;&#x438;&#x43D;&#x434;&#x436;&#x430;&#x43B;&#x43E;&#x43C;</trd>
  <trdgrp lng="zh">
    <trd>&#x63D8; <pr>hu&#xE1;ng</pr></trd>,
    <trd>&#x63D8;&#x6BD5; <pr>y&#xF3;ngb&#xEC;</pr></trd>,
    <trd>&#x63D8;&#x7562; <pr>y&#xF3;ngb&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ponard.0ego">
  <kap><ofc>*</ofc><tld/>ego <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="&AdE;/fundamento/universala_vortaro.html#av-5163">ponard'eg'</url></lok></fnt></kap>
  <snc>
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="sin" cel="lanc.0o">Lanco</ref>:
      <ekz>
        mi alvenis ne pro <esc>ponardegojn&jcirc;etistoj</esc>
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        li levi&gcirc;is el inter la komunumo kaj prenis <tld/>egon en sian manon
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 25:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la tago de la batalo ne trovi&gcirc;is glavo nek <tld/>ego en la manoj de la tuta popolo
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 13:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        indiferente estas, kies <tld/>ego trafos, se nur la besto estos bone trafita
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Pike</trd>,
    <trd>Spie&szlig;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>spear</trd>,
    <trd>lance</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;pieu</trd>,
    <trd>pique</trd>
  </trdgrp>
  <trd lng="uk">&#x441;&#x43F;&#x438;&#x441;</trd>
  <trd lng="zh">&#x77DB; <pr>m&#xE1;o</pr></trd>
</drv>

</art>
