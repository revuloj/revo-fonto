<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>infim</rad>/o
    </kap>
    <drv mrk="infim.0o">
      <kap><tld/>o</kap>
      <snc>
        <uzo tip="fak">MAT</uzo>
        <fnt>PIV</fnt>
        <dif>
          <klr>(de subaro <em>A</em> de orda aro (<em>E</em>, <em>R</em>))</klr>
          La <ref tip="super" cel="maksim1.0o.MAT">maksimumo</ref> de la aro 
          de &gcirc;iaj malsupraj 
          <ref cel="bar.0o.MAT">baroj</ref>; alidire: la plej granda
          malsupra baro:
          <ekz>
            se &gcirc;i ekzistas, la <tld/>o de <em>A</em> povas
            ne aparteni al <em>A</em>;
          </ekz>
          <ekz>
            &ccirc;iu reela subaro kun malsupraj baroj akceptas reelan 
            <tld/>on, 
            sed tio ne validas en la aro de racionalaj nombroj. 
          </ekz>
        </dif>
        <refgrp tip="ant">
          <ref cel="suprem.0o">supremo</ref>.
        </refgrp>
      </snc>
      <trd lng="fr">borne inf&eacute;rieure</trd>
    </drv>
  </art>

<!--
$Log$
-->
</vortaro>