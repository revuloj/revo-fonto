<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>infim</rad>/o
</kap>
<drv mrk="infim.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <klr>(de subaro <em>A</em> de
      <ref cel="ord.0a.aro">orda aro</ref> (<em>E</em>, <em>R</em>))</klr>
      La <ref tip="super" cel="maksim1.0o.MAT">maksimumo</ref> de la aro
      de &gcirc;iaj subaj
      <ref cel="bar.0o.MAT">baroj</ref>; alidire: la plej granda
      suba baro:
      <ekz>
        se &gcirc;i ekzistas, la <tld/>o de <em>A</em> povas
        ne aparteni al <em>A</em>;
      </ekz>
      <ekz>
        &ccirc;iu reela subaro kun subaj baroj akceptas reelan
        <tld/>on,
        sed tio ne validas en la aro de racionalaj nombroj.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="suprem.0o">supremo</ref>.
    </refgrp>
    <rim>
      Kelkaj preferas la pli s&hcirc;emisman <ctl>suba limo</ctl>.
    </rim>
  </snc>
  <trdgrp lng="de">
    <trd>Infimum</trd>,
    <trd>untere <ind>Grenze</ind></trd></trdgrp> 
  <trd lng="en">greatest lower <ind>bound</ind></trd>
  <trd lng="fr">borne inf&eacute;rieure</trd>
  <trd lng="ru">&c_t;&c_o;&c_ch;&c_n;&c_a;&c_ja; &c_n;&c_i;&c_zh;&c_n;&c_ja;&c_ja; &c_g;&c_r;&c_a;&c_n;&c_i;&c_c;&c_a; </trd>
</drv>
</art>
