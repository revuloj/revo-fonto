<vortaro>
<art mrk="$Id: oligohx.xml,v 1.10 2022/03/24 15:12:21 revo Exp $">
<kap>
  <rad>oligo&hcirc;et</rad>/oj <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="oligohx.0oj">
  <kap><tld/>oj</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      Subklaso el la klaso <ref tip="malprt" cel="verm.zono0oj">zonovermoj</ref>
      (<trd lng="la">Oligochaeta</trd>):
      <ekz>
        tervermo estas la komuna nomo por la plej grandaj membroj de la <tld/>oj 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=81016">Tervermo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj <klr>[&#x2026;]</klr> ne havas antenojn, palpilojn, parapodiumojn, sed kelkaj akvaj
        oligo&hcirc;etoj havas brankon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=81033">Oligo&hcirc;etoj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref cel="lumbri.0o">lumbriko</ref>
        <klr>[&#x2026;]</klr> estas genro el familio de <ref tip="prt" cel="lumbri1.0oj">lumbrikedoj</ref>,
        
        subklaso de <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=28535">Lumbriko</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">oligoquets</trd>
  <trd lng="de">Wenigborster</trd>
  <trd lng="fr">oligoch&egrave;tes</trd>
  <trd lng="hu">kev&eacute;ss&ouml;rt&eacute;j&udblac;ek<klr tip="amb"> (rend)</klr></trd>
  <trd lng="nl">borstelarme <ind>ringwormen</ind></trd>
  <trd lng="pl">sk&aogonek;poszczety</trd>
  <trd lng="pt">oligoquetas</trd>
  <trd lng="ro">oligochete </trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_l;&c_i;&c_g;&c_o;&c_h;&c_je;&c_t;&c_y;<klr tip="ind"> (&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>,
    <trd>&c_m;&c_a;&c_l;&c_o;&c_shch;&c_je;&c_t;&c_i;&c_n;&c_k;&c_o;&c_v;&c_y;&c_je; &c_ch;&c_je;&c_r;&c_v;&c_i; <klr tip="amb">(&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x430;&#x43B;&#x43E;&#x449;&#x435;&#x442;&#x438;&#x43D;&#x43A;&#x43E;&#x432;&#x456; &#x43A;&#x456;&#x43B;&#x44C;&#x447;&#x430;&#x43A;&#x438;</trd>,
    <trd>&#x43C;&#x430;&#x43B;&#x43E;&#x449;&#x435;&#x442;&#x438;&#x43D;&#x43A;&#x43E;&#x432;&#x456; &#x447;&#x435;&#x440;&#x432;&#x438;</trd>,
    <trd>&#x43E;&#x43B;&#x456;&#x433;&#x43E;&#x445;&#x435;&#x442;&#x438;</trd>
  </trdgrp>
</drv>
</art>
