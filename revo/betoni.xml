<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: betoni.xml,v 1.23 2023/08/31 08:48:51 revo Exp $">
<kap>
<rad>betonik</rad>/o
</kap>
<drv mrk="betoni.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio 
      <ref tip="malprt" cel="lamiac.0oj">lamiacoj</ref>
      (<trd lng="la">Stachys</trd>);
      <ref tip="lst" cel="veget.0ajxo" lst="voko:lamioparencoj">vegeta&jcirc;o</ref>
      kun kontra&ubreve;sidaj folioj kaj labiaj floroj &scirc;ajne formantaj spikon;
      la kvin kalikfolioj formas tubeton a&ubreve; klo&scirc;on; la korolo konsistas
      el malsupra lipo kaj ne tre kava supra lipo, la stamenoj etendi&gcirc;as el
      la korolo, sed ne estas pli longaj ol la supra lipo kaj kurbi&gcirc;as
      eksteren fine de la flortempo; la trie&gcirc;aj fruktoj disfalas en unusemajn
      &ccirc;elojn &ccirc;e maturi&gcirc;o; pli frue oni nomis betonikoj nur certajn speciojn de la
      genro, ekz. tiuj sen malbonodoro a&ubreve; tiujn kun bazaj rozete aran&gcirc;itaj
      folioj:
      <ekz>
        la <tld/>o trovi&gcirc;as sur herbejodeklivoj kaj je arbarorandoj sur ne tro seka
        <klr>[&#x2026;]</klr> kalkori&ccirc;a grundo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=462543">Betoniko</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/betonik.jpg">
    <ind>arbara <tld/>o</ind>
    <trd lng="ca">ortiga pudent</trd>
    <trd lng="de">Wald-Ziest</trd>
    <trd lng="la">Stachys sylvatica</trd>
    <trd lng="hu">erdei <ind>tisztesf&udblac;</ind></trd>
    <trd lng="sv">stinksyska</trd>
  </bld>
  <trd lng="be">&c_ch;&c_y;&c_s;&c_mol;&c_c;&c_ib;&c_k;</trd>
  <trdgrp lng="ca">
    <trd>bet&ograve;nica</trd>,
    <trd>g&egrave;nere Stachys</trd>
  </trdgrp>
  <trd lng="en">stachys</trd>
  <trdgrp lng="de">
    <trd>Ziest</trd>,
    <trd>Betonie</trd>
  </trdgrp>
  <trd lng="fr">b&eacute;toine</trd>
  <trd lng="hu">tisztesf&udblac;</trd>
  <trd lng="ja">&#x30AB;&#x30C3;&#x30B3;&#x30A6;&#x30C1;&#x30E7;&#x30ED;&#x30AE;</trd>
  <trd lng="pl">bukwica</trd>
  <trd lng="ru">&c_b;&c_u;&c_k;&c_v;&c_i;&c_c;&c_a;</trd>
  <trd lng="sv">syska</trd>
  <trd lng="sk">betonika lek&aacute;rska</trd>
  <trd lng="cs">bukvice l&eacute;ka&rcaron;sk&aacute;</trd>
  <trd lng="uk">&#x431;&#x443;&#x43A;&#x432;&#x438;&#x446;&#x44F;</trd>
</drv>
</art>
