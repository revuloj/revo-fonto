<vortaro>
  <art mrk="$Id: gobi1.xml,v 1.2 2025/03/25 08:36:04 afido Exp $">
    <kap>
      <rad>Gobi</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="gobi1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="gobi1.0o.dezerto">
    <dif>
      <ref tip="super" cel="dezert.0o">Dezerto</ref>, granda seka regiono
      amba&ubreve;flanke de la suda landlimo de <ref tip="malprt" cel="mongol.M0ujo">Mongolujo</ref>
      kun <ref tip="malprt" cel="cxin.CX0ujo">&Ccirc;inujo</ref>:
      <ekz>
        li biciklis <klr>[&#8230;]</klr> la&ubreve;longe de la Flava Rivero kaj atingis Mongolion tra
        la dezerto <tld/>o
        <fnt>
          <bib>LOdE</bib>
          <lok>2004, &#8470; 4 (114)</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">W&uuml;ste <ind>Gobi</ind></trd>
    <trd lng="en"><ind>Gobi</ind> desert</trd>
    <trd lng="fr">d&eacute;sert de <ind>Gobi</ind></trd>
    <trd lng="mn">&c_G;&c_o;&c_v;&c_mol;</trd>
    <trd lng="ru">&c_G;&c_o;&c_b;&c_i;</trd>
    <trd lng="zh">&#25096;&#22721; <pr>G&emacron;b&igrave;</pr></trd>
  </snc>
  <ref tip="hom" cel="gobi.0o">gobio (fi&scirc;o)</ref>
<trd lng="be">&c_G;&c_o;&c_b;&c_ib;</trd>
</drv>

  </art>

