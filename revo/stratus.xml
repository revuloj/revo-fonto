<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>stratus</rad>/o
</kap>
<drv mrk="stratus.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MET</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <fnt>PIV</fnt>
  <snc>
    <dif>
      Nubo formanta kontinuajn horizontalajn tavolojn. Kp <ref
      tip="vid" cel="cirus.0o">ciruso</ref>, <ref
      tip="vid" cel="kumulu.0o">kumuluso</ref> kaj <ref
      tip="vid" cel="nimbus.0o">nimbuso</ref>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_l;&c_a;&c_ib;&c_s;&c_t;&c_a;&c_je; <ind>&c_v;&c_o;&c_b;&c_l;&c_a;&c_k;&c_a;</ind></trd>,
    <trd>&c_s;&c_l;&c_a;&c_ib;&c_s;&c_t;&c_a;&c_ja; <ind>&c_h;&c_m;&c_a;&c_r;&c_a;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schichtwolke</trd>,
    <trd>Stratuswolke</trd>
  </trdgrp>
  <trd lng="es">estrato</trd>
  <trdgrp lng="hu">
    <trd>r&eacute;tegfelh&odblac;</trd>,
    <trd>stratus</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_l;&c_o;&c_i;&c_s;&c_t;&c_o;&c_je; &c_o;&c_b;&c_l;&c_a;&c_k;&c_o;</trd>
</drv>
</art>
