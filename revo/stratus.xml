<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stratus.xml,v 1.16 2023/10/21 07:46:48 revo Exp $">
<kap>
  <rad>stratus</rad>/o   <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="stratus.0o">
  <kap><tld/>o</kap>
  <snc mrk="stratus.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      <ref tip="super" cel="nub.0o.MET">Nubo</ref>
      formanta kontinuajn horizontalajn tavolojn:
      <ekz>
        drezelo a&ubreve; pluveto <klr>[&#x2026;]</klr> origin<klr>[&#x2026;]</klr>as
        el nuboj relative malaltaj kaj de malmulta vertikala disvolvi&gcirc;o
        kiaj estas la <tld/>kumulusoj a&ubreve; <tld/>oj 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=570273">Drezelo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cirus.0o.MET">ciruso</ref>,
      <ref cel="kumulu.0o.MET">kumuluso</ref>,
      <ref cel="nimbus.0o.MET">nimbuso</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_l;&c_a;&c_ib;&c_s;&c_t;&c_a;&c_je; <ind>&c_v;&c_o;&c_b;&c_l;&c_a;&c_k;&c_a;</ind></trd>,
    <trd>&c_s;&c_l;&c_a;&c_ib;&c_s;&c_t;&c_a;&c_ja; <ind>&c_h;&c_m;&c_a;&c_r;&c_a;</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>deskovit&yacute; oblak</trd>,
    <trd>sloha</trd>,
    <trd>stratus</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schichtwolke</trd>,
    <trd>Stratuswolke</trd>
  </trdgrp>
  <trd lng="en">stratus</trd>
  <trd lng="es">estrato</trd>
  <trd lng="fa">&a_A;&a_s;&a_t;&a_r;&a_A;&a_t;&a_w;&a_s;</trd>
  <trd lng="fr">stratus</trd>
  <trdgrp lng="hu">
    <trd>r&eacute;tegfelh&odblac;</trd>,
    <trd>stratus</trd>
  </trdgrp>
  <trd lng="ja">&#x5C64;&#x96F2;  <pr>&#x305D;&#x3046;&#x3046;&#x3093;</pr></trd>
  <trdgrp lng="pl">
    <trd>chmura warstwowa</trd>,
    <trd>stratus <klr>(St)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_l;&c_o;&c_i;&c_s;&c_t;&c_o;&c_je; &c_o;&c_b;&c_l;&c_a;&c_k;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>doskovit&yacute; oblak</trd>,
    <trd>stratus</trd>
  </trdgrp>
  <trd lng="uk">&#x448;&#x430;&#x440;&#x443;&#x432;&#x430;&#x442;&#x430; &#x445;&#x43C;&#x430;&#x440;&#x430;</trd>
</drv>
</art>
