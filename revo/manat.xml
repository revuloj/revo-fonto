<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>manat</rad>/o
</kap>
<drv mrk="manat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio el la ordo <ref tip="super" cel="siren.0oj">sirenoj</ref>
      (<trd lng="la">Trichechus manatus</trd>), vivanta en la
      estuaroj de Amazono kaj Orinoko.
    </dif>
    <refgrp tip="sin">
      <ref cel="delfen.river0o">riverdelfeno</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_m;&c_a;&c_n;&c_t;&c_y;&c_n;</trd>
  <trd lng="de">Manati</trd>
  <trd lng="en">manatee</trd>
  <trdgrp lng="fr">
    <trd>lamantin</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>man&aacute;ti</trd>,
    <trd>lamantin</trd>
  </trdgrp>
  <trd lng="nl">zeekoe</trd>
  <trd lng="ru">&c_l;&c_a;&c_m;&c_a;&c_n;&c_t;&c_i;&c_n;</trd>
</drv>
</art>
