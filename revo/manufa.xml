<vortaro>
<art mrk="$Id: manufa.xml,v 1.17 2020/06/24 21:10:50 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>manufaktur</rad>/o
</kap>
<drv mrk="manufa.0o">
  <kap><tld/>o</kap>
  <snc mrk="manufa.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Granda laborejo, kie oni produktas serie komerca&jcirc;ojn el
      krudaj materialoj,
      precipe per mana a&ubreve; po-peca prilaboro:
      <ekz>
        ceramikaj <tld/>oj
        <fnt>
          <aut>V. I. Ionesov</aut>
          <vrk><url
          ref="http://www.eventoj.hu/steb/arkeologio/baktrio/baktrio1.htm">
          La formado de frupolitikaj rilatoj en la proto-baktria
          civilizacio de la bronza epoko</url></vrk>
          <lok>Scienca Revuo, 1994:2 (45), p. 9a-25a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="fabrik.0o">fabriko</ref>.
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_b;&c_r;&c_y;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_n;&c_u;&c_f;&c_a;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>
  </trdgrp>                   
  <trd lng="br">labouradeg</trd>
  <trdgrp lng="ca">
    <trd>manufactura</trd>,
    <trd>f&agrave;brica</trd>
  </trdgrp>
  <trd lng="de">Manufaktur</trd>
  <trdgrp lng="en">
    <trd>factory</trd>,
    <trd>manufactory</trd>,
    <trd>works</trd>
  </trdgrp>
  <trd lng="fr">manufacture</trd>
  <trd lng="hu">manufakt&uacute;ra</trd>
  <trd lng="nl">fabriek</trd>
  <trd lng="pl">manufaktura</trd>
  <trdgrp lng="pt">
    <trd>manufatura</trd>,
    <trd>f&aacute;brica</trd>
  </trdgrp>
  <trd lng="ro">atelier</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_a;&c_b;&c_r;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_n;&c_u;&c_f;&c_a;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="th">
    <trd>&#3650;&#3619;&#3591;&#3612;&#3621;&#3636;&#3605;</trd>,
    <trd>&#3650;&#3619;&#3591;&#3591;&#3634;&#3609;</trd>
  </trdgrp>
</drv>
</art>
