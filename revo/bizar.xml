<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bizar.xml,v 1.14 2021/03/04 20:39:52 revo Exp $">
<kap>
   <rad>bizar</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="bizar.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <ref tip="super" cel="strang.0a">Stranga</ref>, &scirc;ajne
      <ref tip="sin" cel="absurd.0a">absurda</ref>, nekohere a&ubreve; neharmonie impresa:
      <ekz>
         la mondo neniam konis pli <tld/>an arkitekturon
         <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 25a, p. 263a</lok></fnt>.
      </ekz>
      <ekz>
        <tld/>a miksa&jcirc;o de fiasko kaj festo
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007260.html">Politiko elreligita</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kvankam <klr>[&#x2026;]</klr> iom <tld/>a, rakontado de tiaj timigaj memoroj kaj
        tradicia&jcirc;oj estis populara distra&jcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="https://www.monato.be/2005/008717.php">Bram Stoker, kreinto de Dracula</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a ago de la ju&gcirc;isto povas <klr>[&#x2026;]</klr> doni la impreson, ke oni
        intencas inkludi en la &jcirc;urio nur simpatiantojn de la akuzito
        <fnt>
          <bib>Monato</bib>
          <aut>PGubbins</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006914.html">Irlando: Lando de malsanktuloj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kvar komplete tradukitaj tabulotekstoj malkovras <tld/>ajn detalojn pri moroj
        kaj seksritoj <klr>[&#x2026;]</klr> &ccirc;e la kultvila&gcirc;o Orongo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007315.html">De&ccirc;ifrado de la paskinsula skribo</url></vrk>
        </fnt>;
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="ekscentr.0a.KOMUNE">ekscentra</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_ib;&c_w;&c_n;&c_y;</trd>
  <trd lng="bg">&c_s;&c_t;&c_r;&c_a;&c_n;&c_je;&c_n;</trd>
  <trdgrp lng="ca">
    <trd>estrafolari</trd>,
    <trd>rar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bizarn&iacute;</trd>,
    <trd>podivn&yacute;</trd>,
    <trd>podiv&iacute;nsk&yacute;</trd>,
    <trd>roztodivn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>bizarr</trd>,
     <trd>skurril</trd>
   </trdgrp>
  <trdgrp lng="en">
     <trd>bizarre</trd>,
     <trd>eccentric</trd>,
     <trd>odd</trd>,
     <trd>strange</trd>
   </trdgrp>
  <trdgrp lng="es">
    <trd>raro</trd>,
    <trd>inusual</trd>
  </trdgrp>
  <trd lng="fr">bizarre</trd>
  <trd lng="he">&#x5DE;&#x5D5;&#x5BC;&#x5D6;&#x5B8;&#x5E8;</trd>
  <trdgrp lng="pl">
     <trd>cudaczny</trd>,
     <trd>dziwaczny</trd>
   </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_p;&c_r;&c_i;&c_ch;&c_u;&c_d;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>,
     <trd>&c_d;&c_i;&c_k;&c_o;&c_v;&c_i;&c_n;&c_n;&c_y;&c_j;</trd>,
     <trd>&c_s;&c_t;&c_r;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
     <trd>&c_s;&c_v;&c_o;&c_je;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_j;</trd>
   </trdgrp>
  <trd lng="sk">podivn&yacute;</trd>
</drv>

<drv mrk="bizar.0i">
   <kap><tld/>i</kap>
   <gra><vspec>ntr</vspec></gra>
   <snc>
     <dif>
       Aspekti, konduti eksterkutime:
       <ekz>
         &scirc;i denove kritikos, ke mi <tld/>as
         <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 63a, p.
         369a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_d;&c_z;&c_ib;&c_w;&c_n;&c_a;
     &c_v;&c_y;&c_g;&c_l;&c_ja;&c_d;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_d;&c_z;&c_ib;&c_w;&c_n;&c_a;
     &c_p;&c_a;&c_v;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol;
     &c_s;&c_ja;&c_b;&c_je;</trd>
   </trdgrp>
   <trd lng="ca">ser <ind>estrafolari</ind></trd>
   <trdgrp lng="de">
     <trd>sich <ind>bizarr</ind> verhalten</trd>
   </trdgrp>
   <trd lng="en">be bizarre</trd>
   <trd lng="es">ser inusual</trd>
   <trd lng="fr">sortir de l'ordinaire</trd>
   <trdgrp lng="pl">
     <trd>cudowa&cacute;</trd>,
     <trd>wydziwia&cacute;</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_s;&c_t;&c_r;&c_a;&c_n;&c_n;&c_o; &c_v;&c_je;&c_s;&c_t;&c_i;
     &c_s;&c_je;&c_b;&c_ja;</trd>,
     <trd>&c_ch;&c_u;&c_d;&c_i;&c_t;&c_mol;</trd>
   </trdgrp>
</drv>
</art>

