<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <rad>bizar</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="bizar.0a">
   <kap><tld/>a</kap>
   <snc>
     <dif>
       <ref tip="super" cel="strang.0a">Stranga</ref>, &scirc;ajne absurde,
       nekohere a&ubreve; neharmonie impresa:
       <ekz>
         la mondo neniam konis pli <tld/>an arkitekturon<fnt><bib>MkM</bib>,
         <lok>&ccirc;apitro 25a, p. 263a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="fr">bizarre</trd>
</drv>

<drv mrk="bizar.0i">
   <kap><tld/>i</kap>
   <gra><vspec>ntr</vspec></gra>
   <snc>
     <dif>
       Aspekti, konduti eksterkutime:
       <ekz>
         &scirc;i denove kritikos, ke mi <tld/>as
         <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 63a, p. 369a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="fr">sortir de l'ordinaire</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>