<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: muskol.xml,v 1.30 2023/09/03 09:23:19 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>muskol</rad>/o
</kap>

<drv mrk="muskol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="organ.0o.BIO">Organo</ref> konsistanta el
      karnaj fibroj kaj servanta por movi la diversajn partojn de la
      korpo:
      <ekz>
        &ccirc;iutagaj ekzercoj por milde stre&ccirc;i la &scirc;ultrajn, brakajn kaj
        dorsajn <tld/>ojn
        <fnt>
          <aut>David Curtis</aut>
          <vrk><url ref="&Monato;/2002/pdf/monato200201.pdf#page=28">Koratako</url></vrk>
          <bib>Monato</bib>
          <lok>2002/01, p. 28-34</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_u;&c_s;&c_k;&c_u;&c_l;</trd>,
    <trd>&c_c;&c_ja;&c_g;&c_l;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">sval</trd>
  <trd lng="de">Muskel</trd>
  <trd lng="en">muscle</trd>
  <trd lng="es">m&uacute;sculo</trd>
  <trd lng="fr">muscle</trd>
  <trd lng="hu">izom</trd>
  <trdgrp lng="id">
    <trd>otot</trd>,
    <trd>urat</trd>
  </trdgrp>
  <trd lng="it">muscolo</trd>
  <trdgrp lng="ja">
    <trd>&#x7B4B;&#32905;  [<ind>&#12377;&#12376;&#x306B;&#x304F;</ind>]</trd>,
    <trd>&#x7B4B;  [<ind>&#x304D;&#12435;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">spier</trd>
  <trd lng="pl">mi&eogonek;sie&nacute;</trd>
  <trd lng="pt">m&uacute;sculo <klr tip="amb">(anat.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_y;&c_sh;&c_c;&c_a;</trd>,
    <trd>&c_m;&c_u;&c_s;&c_k;&c_u;&c_l;</trd>
  </trdgrp>
  <trd lng="sk">sval</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#8217;&#x44F;&c_z;</trd>,
    <trd>&#x43C;&c_u;&c_s;&#x43A;&c_u;&#x43B;</trd>
  </trdgrp>
</drv>

<drv mrk="muskol.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>oj, al korpa forto kiun ili provizas:
      <ekz>
        <ctl><tld lit="M"/>a belulino</ctl>
        <fnt>
        <aut>JPVDD</aut>,
        <vrk><url ref="&Monato;/1995/monato199507.pdf#page=27">&Scirc;paru monon</url> (recenzo pri
        Vinilkosmokompil)</vrk>,
        <bib>Monato</bib>,
        <lok>1995/07, p. 27</lok>
        </fnt>;
      </ekz>
      <ekz>
        Creuzfeldt-Jakob-malsano manifesti&gcirc;as kiel nekuracebla degenero
        de
        nervoj, rezultigante perdon de mensaj kaj <tld/>aj
        kapabloj
        <fnt>
        <aut>Krys Ungar</aut>,
        <vrk><url ref="&Monato;/1995/004583.php">Vivu vigle kaj vegetare!</url></vrk>,
        <bib>Monato</bib>,
        <lok>1995/07, p. 18</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_s;&c_k;&c_u;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>svalnat&yacute;</trd>,
    <trd>svalov&yacute;</trd>
  </trdgrp>
  <trd lng="de">muskul&ouml;s</trd>
  <trdgrp lng="es">
    <trd>muscular</trd>,
    <trd>musculoso</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>musculaire</trd>,
    <trd>muscl&eacute;</trd>,
    <trd>musculeux</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>izom-</trd>,
    <trd>muszkul&aacute;ris</trd>,
    <trd>izmos</trd>
  </trdgrp>
  <trd lng="id">ber<ind>otot</ind></trd>
  <trd lng="it">muscolare</trd>
  <trdgrp lng="ja">
    <trd>&#x7B4B;&#32905;&#x306E;  [<ind>&#x304D;&#12435;&#x306B;&#x304F;&#x306E;</ind>]</trd>,
    <trd>&#x7B4B;&#32905;&#x306E;&#x591A;&#12356; 
    [<ind>&#x304D;&#12435;&#x306B;&#x304F;&#x306E;&#x304A;&#x304A;&#12356;</ind>]</trd>,
    <trd>&#x7B4B;&#x9AA8;&#x305F;&#x304F;&#x307E;&#12375;&#12356; 
    [<ind>&#x304D;&#12435;&#12371;&#12388;&#x305F;&#x304F;&#x307E;&#12375;&#12356;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gespierd</trd>,
    <trd>spier-</trd>
  </trdgrp>
  <trd lng="pl">mi&eogonek;&sacute;niowy</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_y;&c_sh;&c_je;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_m;&c_u;&c_s;&c_k;&c_u;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>muskul&aacute;rny</trd>,
    <trd>svalov&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&#8217;&#x44F;&c_z;&#x43E;&c_v;&c_i;&c_j;</trd>,
    <trd>&#x43C;&c_u;&c_s;&#x43A;&c_u;&#x43B;&#x44C;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="muskol.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="muskol.0aro.SNC">
    <dif>
      Tuto de la <tld/>oj de besto a&ubreve; homo:
      <ekz>
         la movo de glata <tld/>aro estas sen intenca volo
         <fnt>
           <aut>Vikipedio</aut>,
           <vrk><url ref="&Viki;/Glata_muskolo">Glata
           muskolo</url></vrk>
         </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_s;&c_k;&c_u;&c_l;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>muskulatura</trd>,
    <trd>svalstvo</trd>,
    <trd>svaly</trd>
  </trdgrp>
  <trd lng="fr">musculature</trd>
  <trdgrp lng="ja">
    <trd>&#x7B4B;&#32905;&#x7D44;&#x7E54;  [<ind>&#x304D;&#12435;&#x306B;&#x304F;&#x305D;&#12375;&#x304D;</ind>]</trd>,
    <trd>&#x7B4B;&#x7CFB;  [<ind>&#12377;&#12376;&#12369;&#12356;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>musculatuur</trd>,
    <trd>spierstelsel</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>svalstvo</trd>,
    <trd>svaly</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&#8217;&#x44F;&c_z;&c_i;</trd>,
    <trd>&#x43C;&c_u;&c_s;&#x43A;&c_u;&#x43B;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
  </trdgrp>
</drv>
</art>
