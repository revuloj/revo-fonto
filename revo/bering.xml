<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Bering</rad>/o
    </kap>
    <drv mrk="bering.B0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">PERS</uzo>
      <snc>
        <dif>
          Dandevena maristo kaj esploristo (Vitus Jonassen <tld/>,
          1681-1741, en la rusa servo ekde 1703), malkovrinta markolon
          inter Azio kaj Ameriko (<ref cel="bering.B0aMarkolo"><tld/>a
            markolo</ref>) kaj Alaskon; anka&ubreve; la
          <ref cel="bering.B0aMaro"><tld/>a Maro</ref> kaj la insulo kie li
          mortis (la pli granda el la Komandoraj insuloj) portas lian nomon.
        </dif>
      </snc>
      <trd lng="be">&c_B;&c_e;&c_r;&c_y;&c_n;&c_g;</trd>
      <trd lng="de">Bering</trd>
      <trd lng="en">Bering</trd>
      <trd lng="es">Bering</trd>
      <trd lng="hu">Bering</trd>
      <trd lng="nl">Bering</trd>
      <trd lng="ru">&c_B;&c_je;&c_r;&c_i;&c_n;&c_g;</trd>
    </drv>
    <drv mrk="bering.B0aMarkolo">
      <kap><tld/>a Markolo</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
        <dif>
          Markolo disiganta <ref cel="azi.A0o">Azion</ref> disde
          <ref cel="amerik.A0o">Ameriko</ref>,
          kaj liganta la Arktan Oceanon kun Pacifiko (169&ring;W, 65&ring;N).
        </dif>
      </snc>
      <bld lok="../bld/Beringa.png">
        La <tld/>aj Markolo kaj Maro
        <fnt><lok>La&ubreve; mapo el <url
 ref="http://www.pmel.noaa.gov/bering/pages/geog.html">NOAA</url>.</lok></fnt>
      </bld>
      <trd lng="be">&c_B;&c_e;&c_r;&c_y;&c_n;&c_g;&c_a;&c_v;&c_a; &c_p;&c_r;&c_a;&c_t;&c_o;&c_k;&c_a;</trd>
      <trd lng="de">Beringstra&szlig;e</trd>
      <trd lng="en">Bering Strait</trd>
      <trd lng="es">Estrecho de Bering</trd>
      <trd lng="hu">Bering-szoros</trd>
      <trd lng="nl">Beringstraat</trd>  
      <trd lng="ru">&c_B;&c_je;&c_r;&c_i;&c_n;&c_g;&c_o;&c_v; &c_p;&c_r;&c_o;&c_l;&c_i;&c_v;</trd>
    </drv>
    <drv mrk="bering.B0aMaro">
      <kap><tld/>a Maro</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
        <dif>
          La plej norda maro
          <ref tip="malprt" cel="pacifik.0o">Pacifika</ref>.
        </dif>
      </snc>
      <trd lng="be">&c_B;&c_e;&c_r;&c_y;&c_n;&c_g;&c_a;&c_v;&c_a; &c_m;&c_o;&c_r;&c_a;</trd>
      <trd lng="de">Beringsee</trd>
      <trd lng="en">Bering Sea</trd>
      <trd lng="hu">Bering-tenger</trd>
      <trd lng="nl">Beringzee</trd>
      <trd lng="ru">&c_B;&c_je;&c_r;&c_i;&c_n;&c_g;&c_o;&c_v;o &c_m;&c_o;&c_r;&c_je;</trd>
    </drv>
  </art>

