<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bering.xml,v 1.22 2023/08/31 08:48:50 revo Exp $">
<kap>
  <rad>Bering</rad>/o
</kap>
<drv mrk="bering.B0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">PERS</uzo>
    <dif>
      Dandevena <ref tip="lst" lst="voko:historiaj_personoj" cel="mar.0isto">maristo</ref>
      kaj esploristo (<nom>Vitus Jonassen</nom> <trd lng="da">Bering</trd>,
      1681-1741, en la rusa servo ekde 1703), malkovrinta markolon
      inter Azio kaj Ameriko (<ref cel="kol.BeringaMar0o"><tld/>a
      Markolo</ref>) kaj Alaskon:
      <ekz>
        la <ref cel="mar.BeringaM0o"><tld/>a Maro</ref> kaj la 
        <ref tip="lst" cel="insul.0o" lst="voko:insuloj">insulo</ref> kie
        li mortis (la pli granda el la Komandoraj insuloj) portas la nomon
        de <tld/>o.
       </ekz>
    </dif>
  </snc>
  <bld lok="../bld/Beringa.png">
    La <tld/>aj Markolo kaj Maro
    <fnt><lok>La&ubreve; mapo el
    <url ref="http://www.pmel.noaa.gov/bering/pages/geog.html">NOAA</url>.
    </lok></fnt>
  </bld>
  <trd lng="be">&c_B;&c_e;&c_r;&c_y;&c_n;&c_g;</trd>
  <trd lng="ca">Bering</trd>
  <trd lng="de">Bering</trd>
  <trd lng="en">Bering</trd>
  <trd lng="es">Bering</trd>
  <trd lng="fr">Bering</trd>
  <trd lng="hu">Bering</trd>
  <trd lng="id">Bering</trd>
  <trd lng="it">Bering</trd>
  <trd lng="ja">&#x30D9;&#x30FC;&#x30EA;&#x30F3;&#x30B0;</trd>
  <trd lng="nl">Bering</trd>
  <trd lng="pl">Bering</trd>
  <trd lng="ru">&c_B;&c_je;&c_r;&c_i;&c_n;&c_g;</trd>
  <trd lng="sk">Bering V.</trd>
  <trd lng="cs">Bering V.</trd>
  <trd lng="uk">&#x411;&#x435;&#x440;&#x456;&#x43D;&#x491;</trd>
</drv>

</art>
