  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_z;&c_o;&c_n;</trd>,
    <trd>&c_l;&c_u;&c_zh;&c_a;&c_j;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sv">gr&auml;smatta</trd>
</drv>
