<art mrk="$Id: gazon.xml,v 1.23 2023/11/11 10:00:39 revo Exp $">
<kap>
  <rad>gazon</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="gazon.0o">
  <kap><tld/>o</kap>
  <snc mrk="gazon.0o.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      Zorge kultivata <ref tip="super" cel="herb.0ejo">herbejo</ref>
      precipe ornama:
      <ekz>
        kiom da tempo ili bezonos por fal&ccirc;i la <tld/>on
        <fnt>
        <aut>Jomo Ipfelkofer</aut>,
        <vrk>Komparo de instrumetodoj</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2001a, numero 6a, p. 9a</lok>
        </fnt>?
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="tapisx.herbo0o">herbotapi&scirc;o</ref>,
      <ref cel="razen.0o">razeno</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_z;&c_o;&c_n;</trd>
  <trd lng="bg">&c_t;&c_r;&c_je;&c_v;&c_n;&c_a; &c_p;&c_l;&c_o;&c_shch;</trd>
  <trd lng="ca">gespa</trd>
  <trd lng="cs">tr&#xE1;vn&#xED;k p&#x11B;st&#x11B;n&#xFD;</trd>
  <trd lng="de">Rasen</trd>
  <trd lng="en">lawn</trd>
  <trd lng="es">c&eacute;sped</trd>
  <trdgrp lng="fr">
    <trd>gazon<klr> (pelouse)</klr></trd>,
    <trd>pelouse</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>p&aacute;zsit</trd>,
    <trd>gyep</trd>
  </trdgrp>
  <trd lng="ja">&#x829D;&#x751F;  <pr>&#x3057;&#x3070;&#x3075;</pr></trd>
  <trdgrp lng="nl">
    <trd>gazon</trd>,
    <trd>grasperk</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>trawnik</trd>,
    <trd>gazon</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>gramado</trd>, 
   <trd>relvado</trd>, 
   <trd>relvagem</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_z;&c_o;&c_n;</trd>,
    <trd>&c_l;&c_u;&c_zh;&c_a;&c_j;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">tr&#xE1;vnik</trd>
  <trd lng="sv">gr&auml;smatta</trd>
  <trd lng="tr">&ccedil;imen</trd>
  <trdgrp lng="uk">
    <trd>&c_gu;&c_a;&c_z;&#x43E;&#x43D;</trd>,
    <trd>&c_d;&c_je;&c_r;&c_je;&#x43D;</trd>
  </trdgrp>
  <trd lng="zh">&#x8349;&#x576A; <pr>c&#x1CE;op&#xED;ng</pr></trd>
</drv>
</art>
