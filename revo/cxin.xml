<kap>
  <ofc>9</ofc>
  <rad>&ccirc;in</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="cxin.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="cxin.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="cxin.CX0ujo"><tld lit="&Ccirc;"/>ujo</ref>:
      <ekz>
        &scirc;i helpis anka&ubreve; al elmigrintaj <tld/>oj
        <fnt>
          <vrk><url ref="&Monato;/1993/A930902.php">Noticoj</url></vrk>
          <bib>Monato</bib>
          <lok>1993/09, p. 4</lok>
        </fnt>;
      </ekz>
      <ekz>
        pli ol unu miliardo da <tld/>oj
        <fnt>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url ref="&Monato;/2000/006852.php">&Ccirc;u vere kristana erao?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/04, p. 17</lok>
          </fnt>;
      </ekz>
      <ekz>
        la <tld/>oj festas <nom>novjaron</nom> la&ubreve; la malnova luna kalendaro, sed en &ccirc;iutagaj aferoj
        ili observas la gregorian
        <fnt>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url ref="&Monato;/2000/006852.php">&Ccirc;u vere kristana erao?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/04, p. 17</lok>
          </fnt>;
      </ekz>
      <ekz>
        la <tld/>ino <nom>Hou Jifan</nom>, aktuala &ccirc;ampiono, kaj la baratanino
        <nom>Koneru Humpy</nom> renkonti&gcirc;is dek fojojn dum la finalo
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2012/010578.php">&Scirc;ak-&scirc;oko por Gorba&ccirc;ov</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">Chinese (citizen)</trd>
    <trd lng="hu">k&iacute;nai <klr tip="amb">(&aacute;llampolg&aacute;r)</klr></trd>
    <trd lng="zh">&#20013;&#22269;&#20154; <pr>zh&omacron;ng gu&oacute; r&eacute;n</pr></trd>
  </snc>
  <snc mrk="cxin.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco; <ref tip="sin" cel="han.0o">hano</ref>.
    </dif>
    <trd lng="de">Chinese</trd>
    <trd lng="el">&kappa;&jota;&ny;&epsilon_ton;&zeta;&omikron;&sigma_fina;
    </trd>
    <trd lng="en">Chinese</trd>
    <trd lng="es">chino</trd>
    <trd lng="he">&samekh;&yod;&nun;&yod;</trd>
    <trd lng="hu">k&iacute;nai <klr tip="amb">(nemzetis&eacute;g&udblac;)</klr></trd>
    <trd lng="nl">Chinees<klr tip="ind"> (persoon)</klr></trd>
    <trd lng="pt">chin&ecirc;s</trd>
    <trd lng="ru">&c_k;&c_i;&c_t;&c_a;&c_je;&c_c;</trd>
    <trd lng="sk">&Ccaron;&iacute;&ncaron;an</trd>
    <trdgrp lng="zh">
      <trd>&#20013;&#21326;&#27665;&#26063; <pr>zh&omacron;ng hu&aacute; m&iacute;n z&uacute;</pr></trd>,
      <trd>&#21326;&#20154; <pr>hu&aacute; r&eacute;n</pr></trd>
    </trdgrp>
  </snc>
  <ref tip="sin" cel="hxin.0o">&hcirc;ino</ref>
  <trd lng="be">&c_k;&c_ib;&c_t;&c_a;&c_je;&c_c;</trd>
  <trd lng="bo">&t_ra;&t_gata;&t_yata;&t_tsheg;&t_ma;&t_gigu;&t_tsheg;</trd>
  <trd lng="ca">xin&eacute;s<klr> (subst.)</klr></trd>
  <trd lng="fr">Chinois<klr> (subst.)</klr></trd>
  <trd lng="it">cinese<klr tip="ind"> (sost.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#20013;&#22269;&#20154;  <pr>&#12385;&#12421;&#12358;&#12372;&#12367;&#12376;&#12435;</pr></trd>,
    <trd>&#28450;&#20154;  <pr>&#12363;&#12425;&#12403;&#12392;</pr></trd>
  </trdgrp>
  <trd lng="pl">Chi&nacute;czyk</trd>
  <trd lng="uk">&c_k;&c_i;&c_t;&c_a;&c_jeu;&c_c;&c_mol;</trd>
</drv>

<drv mrk="cxin.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato:
      <ekz>
        la <tld/>a registaro
        <fnt><aut>Mu Binghua</aut>,
        <vrk><url ref="&Monato;/2000/006919.php">Konsumado dum ferioj kaj vesperoj</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/09, p. 10</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a mediprotekta sektoro
        <fnt>
          <aut>Mu Binghua</aut>
            <vrk><url ref="&Monato;/2001/007256.php">Mediprotekta industrio havas esperigan
            estonton</url></vrk>
            <bib>Monato</bib>
            <lok>2001/02, p. 10</lok>
          </fnt>.
      </ekz>
    </dif>
    <trd lng="en">Chinese<klr> (adj.)</klr></trd>
    <trdgrp lng="fr">
     <trd>chinois</trd>
     <trd>chinoise<klr> (adj.)</klr></trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      <tld lit="&Ccirc;"/>a lingvo:
      <ekz>
         la <tld/>a kune kun la tibeta estas el la sino- a&ubreve; <tld/>o-tibeta familio
        <fnt>
          <aut>Sabeo Fu/Yamasaki Seik&ocirc;</aut>
            <vrk><url ref="&Monato;/2001/007025.php">Formortanta lingvo de malplimulto en
            &Ccirc;inio</url></vrk>
            <bib>Monato</bib>
            <lok>2001/01, p. 22</lok>
          </fnt>.
      </ekz>
    </dif>
    <trd lng="en">Chinese<klr> (language)</klr></trd>
    <trd lng="fr">chinois<klr> (langue)</klr></trd>
  </snc>
  <ref tip="sin" cel="hxin.0a">&hcirc;ina</ref>
  <trd lng="be">&c_k;&c_ib;&c_t;&c_a;&c_j;&c_s;&c_k;&c_ib;</trd>
  <trd lng="ca">xin&eacute;s<klr> (adj.)</klr></trd>
  <trd lng="de">chinesisch</trd>
  <trd lng="he">&samekh;&#1460;&yod;&nun;&#1460;&yod;&tav;</trd>
  <trd lng="hu">k&iacute;nai</trd>
  <trd lng="it">cinese<klr tip="ind"> (agg.)</klr></trd>
  <trd lng="ja">&#20013;&#22269;&#12398;  <pr>&#12385;&#12421;&#12358;&#12372;&#12367;&#12398;</pr></trd>
  <trd lng="pl">chi&nacute;ski</trd>
  <trd lng="ro">chinez</trd>
  <trd lng="uk">&c_k;&c_i;&c_t;&c_a;&c_j;&c_s;&c_mol;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="cxin.CX0ujo">
  <kap><ofc>9</ofc><tld lit="&Ccirc;"/>ujo  <fnt><bib>PIV1</bib></fnt></kap>
  <snc mrk="cxin.CX0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
     <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&Scirc;tato</ref>
      en <ref tip="malprt" cel="azi.OrientaA0o">Orienta
      Azio</ref>,
      &ccirc;e la
      <ref cel="mar.FlavaM0o">Flava Maro</ref>, la
      <ref cel="mar.OrientaCXinaM0o">Orienta &Ccirc;ina Maro</ref> kaj la
      <ref cel="mar.SudaCXinaM0o">Suda &Ccirc;ina Maro</ref>,
      inter <ref cel="rus.R0ujo">Rusujo</ref> kaj
     <ref cel="birm.B0o">Birmo</ref>.
      <!-- La plej granda lando en la
      <ref tip="malprt" cel="azi.InternaA0o">Interna</ref> kaj
      Sud-<ref tip="malprt" cel="azi.OrientaA0o">Orienta Azio</ref>. -->
      <ekz>
        la Rusa Imperio, la <nom>Qing</nom>-Dinastio de <tld lit="&Ccirc;"/>ujo kaj aliaj potencoj
        vasti&gcirc;is en la areon kaj kaptis la &ccirc;iomon da Centra Azio je la fino de la 19-a jarcento
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=722">Centra Azio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        Bambuo <klr>[&#8230;]</klr> <esc>plejparte</esc> kreskas en la humidaj grundoj de Hindujo,
        Tajlando, <tld lit="&Ccirc;"/>ujo kaj Japanujo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=40002">Bambuo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="cxini.0o">&Ccirc;inio</ref>,
      <ref cel="hxin.HX0ujo">&Hcirc;inujo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_K;&c_ib;&c_t;&c_a;&c_j;</trd>
  <trdgrp lng="ca">
    <trd>la Xina</trd>,
    <trd>Rep&uacute;blica Popular de la Xina</trd>
  </trdgrp>
  <trd lng="de">China</trd>
  <trd lng="el">&Kappa;&jota_ton;&ny;&alfa;</trd>
  <trd lng="en">China</trd>
  <trd lng="es">China</trd>
  <trdgrp lng="fr">
    <trd>Chine</trd>,
    <trd>R&eacute;publique populaire de Chine</trd>
  </trdgrp>
  <trd lng="he">&samekh;&yod;&fnun;</trd>
  <trd lng="hu">K&iacute;na</trd>
  <trdgrp lng="id">
    <trd>Cina</trd>,
    <trd>Tiongkok</trd>
  </trdgrp>
  <trd lng="it">Cina</trd>
  <trd lng="nl">China</trd>
  <trd lng="pl">Chiny</trd>
  <trd lng="pt">China</trd>
  <trd lng="ro">China</trd>
  <trd lng="ru">&c_K;&c_i;&c_t;&c_a;&c_j;</trd>
  <trd lng="sk">&Ccaron;&iacute;na</trd>
  <trd lng="uz">&c_H;&c_i;&c_t;&c_o;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#20013;&#22269; <pr>Zh&omacron;nggu&oacute;</pr></trd>,
    <trd><klr>tr. </klr>&#20013;&#22283; [<ind>Zh&omacron;nggu&oacute;</ind>] </trd>,
    <trd>&#20013;&#21326; <pr>Zh&omacron;ng hu&aacute;</pr></trd>,
    <trd><klr>tr. </klr>&#20013;&#33775; <pr>Zh&omacron;ng hu&aacute;</pr></trd>
  </trdgrp>
</drv>

