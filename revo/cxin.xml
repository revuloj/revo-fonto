<drv mrk="cxin.Hindo0ujo">
  <kap>
    <ofc>10</ofc>Hindo<tld/>ujo
  </kap>
  <snc>
    <dif>
      La duoninsulo en sudorienta Azio, kiu situas sude de &Ccirc;inujo kaj oriente de
      Hindujo.
    </dif>
  </snc>
  <trd lng="be">&c_Ib;&c_n;&c_d;&c_a;&c_k;&c_ib;&c_t;&c_a;&c_j;</trd>
  <trd lng="de">Indochina</trd>
  <trdgrp lng="en">
    <trd>Mainland Southeast Asia</trd>,
    <trd>Indochina</trd>
  </trdgrp>
  <trd lng="fr">Indochine</trd>
  <trd lng="pl">Indochiny</trd>
</drv>
