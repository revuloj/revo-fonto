<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>9</ofc>
  <rad>&ccirc;in</rad>/o
</kap>
<drv mrk="cxin.0o">
  <kap><ofc>9</ofc><tld/>o<fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url ref="http://www.akademio-de-esperanto.org/decidoj/9oa.html">
9a Oficiala Aldono</url></vrk>, <lok>&ccirc;in/o</lok></fnt>,
  <var><kap><tld/>ino</kap></var></kap>
  <snc mrk="cxin.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatan(in)o</ref> de
      <ref tip="malprt" cel="cxin.CX0ujo"><tld lit="&Ccirc;"/>ujo</ref>.
    </dif>
    <trd lng="en">Chinese (citizen)</trd>
    <trd lng="hu">k&iacute;nai <klr 
tip="amb">(&aacute;llampolg&aacute;r)</klr></trd>
    <trd lng="zh">&#20013;&#22269;&#20154; [<ind>zh&#333;ng gu&oacute; r&eacute;n</ind>]</trd>
  </snc>
  <snc mrk="cxin.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco.
    </dif>
    <trd lng="be">&c_k;&c_ib;&c_t;&c_a;&c_je;&c_c;</trd>
    <trd lng="de">Chinese</trd>
    <trd lng="el">&kappa;&jota;&ny;&epsilon_ton;&zeta;&omikron;&sigma_fina;
    </trd>
    <trd lng="en">Chinese</trd>
    <trd lng="es">chino</trd>
    <trd lng="he">&samekh;&yod;&nun;&yod;</trd>
    <trd lng="hu">k&iacute;nai <klr 
tip="amb">(nemzetis&eacute;g&udblac;)</klr></trd>
    <trd lng="nl">Chinees<klr tip="ind"> (persoon)</klr></trd>
    <trd lng="pt">chin&ecirc;s</trd>
    <trd lng="ru">&c_k;&c_i;&c_t;&c_a;&c_je;&c_c;</trd>
    <trd lng="sk">&Ccaron;&iacute;&ncaron;an</trd>
    <trdgrp lng="zh">
      <trd>&#20013;&#21326;&#27665;&#26063; [<ind>zh&#333;ng hu&aacute; m&iacute;n z&uacute;</ind>]</trd>,
      <trd>&#21326;&#20154; [<ind>hu&aacute; r&eacute;n</ind>]</trd>
    </trdgrp>
  </snc>
  <trd lng="ca">xin&eacute;s<klr> (subst.)</klr></trd>
  <trd lng="fr">Chinois<klr> (subst.)</klr></trd>
  <trd lng="it">cinese<klr tip="ind"> (sost.)</klr></trd>
  <trd lng="pl">chi&nacute;czyk</trd>
</drv>

<drv mrk="cxin.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato.
    </dif>
    <trd lng="en">Chinese<klr> (adj.)</klr></trd>
    <trdgrp lng="fr"> 
     <trd>chinois</trd>
     <trd>chinoise<klr> (adj.)</klr></trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
    "la &ccirc;ina" = la nacia <ref tip="super" cel="lingv.0o">lingvo</ref> de
     &Ccirc;inujo
    <ekz><fnt><bib>Viki</bib>, <lok><url 
    ref="http://eo.wikipedia.org/wiki/&Ccirc;ina lingvo">&Ccirc;ina
    lingvo</url></lok></fnt></ekz>.
    </dif>
   <trd lng="en">Chinese<klr> (language)</klr></trd>
   <trd lng="fr">chinois<klr> (langue)</klr></trd>
  </snc>  <trd lng="ca">xin&eacute;s<klr> (adj.)</klr></trd>
  <trd lng="hu">k&iacute;nai</trd>
  <trd lng="it">cinese<klr tip="ind"> (agg.)</klr></trd>
  <trd lng="pl">chi&nacute;ski</trd>
  <trd lng="ro">chinez</trd>
</drv>

<drv mrk="cxin.CX0ujo">
  <kap><ofc>9</ofc><tld lit="&Ccirc;"/>ujo
  (<var><kap><tld lit="&Ccirc;"/>/i/o</kap></var>)
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>
    <fnt><bib>Viki</bib>, <lok><url 
    ref="http://eo.wikipedia.org/wiki/&Ccirc;inio">&Ccirc;inio</url></lok></fnt>
  </kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
     <uzo tip="fak">POL</uzo>
    <dif>
      Lando en <ref tip="malprt" cel="azi.OrientaA0o">Orienta
      Azio</ref>,
      &ccirc;e la 
      <ref cel="mar.FlavaM0o">Flava Maro</ref>, la
      <ref cel="mar.OrientaCXinaM0o">Orienta &Ccirc;ina Maro</ref> kaj la
      <ref cel="mar.SudaCXinaM0o">Suda &Ccirc;ina Maro</ref>,
      inter <ref cel="rus.R0ujo">Rusujo</ref> kaj 
     <ref cel="birm.B0o">Birmo</ref>.
      <!-- La plej granda lando en la
      <ref tip="malprt" cel="azi.InternaA0o">Interna</ref> kaj
      Sud-<ref tip="malprt" cel="azi.OrientaA0o">Orienta Azio</ref>. -->
    </dif>
   <ref tip="sin" cel="cxini.0o">&Ccirc;inio</ref>
  </snc>
  <rim mrk="cxin.CX0io.RIM">
  "Gentobazaj landnomoj. Oni ordinare formas la nomon de la lando per la
  <em>Fundamenta sufikso UJ</em> a&ubreve; per la <em>neoficiala sufikso
  I</em>. Pri la karaktero de tiu I sufikso (uzata post popolvortoj
  anstata&ubreve; UJ) ekzistas diversaj opinioj. Iuj rigardas &gcirc;in kiel
  <em>efektivan sufikson</em> (kvankam neoficialan), dum aliaj preferas
  rigardi &gcirc;in kiel <em>eron de aparta landnoma radiko</em> paralela kun
  la responda popolnoma radiko. 
  Tiuj diversaj analizoj &ccirc;iuokaze ne faras praktikan diferencon en la
  uzado de tiaj landnomoj kiel Francio, Rusio, Japanio
  kaj &Ccirc;inio. <em>En tiu &ccirc;i dokumento la Akademio ne alprenas starpunkton
  pri tiu teoria demando</em>" <fnt><aut>Akademio de Esperanto</aut>, 
  <vrk><url
  ref="http://www.akademio-de-esperanto.org/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
  Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>.
  </rim>
  <trd lng="be">&c_K;&c_ib;&c_t;&c_a;&c_j;</trd>
  <trdgrp lng="ca">
    <trd>la Xina</trd>,
    <trd>Rep&uacute;blica Popular de la Xina</trd>
  </trdgrp>
  <trd lng="de">China</trd>
  <trd lng="el">&Kappa;&jota_ton;&ny;&alfa;</trd>
  <trd lng="en">China</trd>
  <trd lng="es">China</trd>
  <trdgrp lng="fr">
    <trd>Chine</trd>,
    <trd>R&eacute;publique populaire de Chine</trd>
  </trdgrp>
  <trd lng="he">&samekh;&yod;&fnun;</trd>
  <trd lng="hu">K&iacute;na</trd>
  <trdgrp lng="id">
    <trd>Cina</trd>,
    <trd>Tiongkok</trd>
  </trdgrp>
  <trd lng="it">Cina</trd>
  <trd lng="nl">China</trd>
  <trd lng="pl">Chiny</trd>
  <trd lng="pt">China</trd>
  <trd lng="ro">China</trd>
  <trd lng="ru">&c_K;&c_i;&c_t;&c_a;&c_j;</trd>
  <trd lng="sk">&Ccaron;&iacute;na</trd>
  <trdgrp lng="zh">
    <trd>&#20013;&#22269; [<ind>Zh&#333;nggu&oacute;</ind>]</trd>,
    <trd><klr>tr. </klr>&#20013;&#22283; [<ind>Zh&#333;nggu&oacute;</ind>] </trd>,
    <trd>&#20013;&#21326; [<ind>Zh&#333;ng hu&aacute;</ind>]</trd>,
    <trd><klr>tr. </klr>&#20013;&#33775; [<ind>Zh&#333;ng hu&aacute;</ind>]</trd>
  </trdgrp>
</drv>

<drv mrk="cxin.0ujano">
  <kap><tld/>ujano,
  <var><kap><tld/>ujanino</kap></var></kap>
  <snc>
    <dif>
      &Scirc;tatano de <tld lit="&Ccirc;"/>ujo.
    </dif>
  </snc>
  <trd lng="ca">xin&eacute;s<klr> (subst.)</klr></trd>
  <trd lng="en">Chinese (citizen)</trd>
  <trd lng="fr">Chinois<klr> (citoyen)</klr></trd>
  <trd lng="hu">k&iacute;nai <klr tip="amb">(K&iacute;n&aacute;ban 
lak&oacute;)</klr></trd>
  <trd lng="it">cinese<klr tip="ind"> (sost.)</klr></trd>
  <trd lng="pl">chi&nacute;czyk</trd>
</drv>

</art>
