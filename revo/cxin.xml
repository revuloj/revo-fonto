<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>&ccirc;in</rad>/o
    </kap>

    <drv mrk="cxin.0o">
      <kap><tld/>o</kap>
      <snc>
	<dif>
	  Ano de la plej multenombra popolo en la mondo, la &ccirc;efa etno
	  de <tld lit="&Ccirc;"/>ujo.
	</dif>
      </snc>
      <trd lng="ru">&c_k;&c_i;&c_t;&c_a;&c_je;&c_c;</trd>
    </drv>

    <drv mrk="cxin.Cx0ujo">
      <kap><tld lit="&Ccirc;"/>ujo</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
	<dif>
	  La plej granda lando en la
          <ref tip="malprt" cel="azi.InternaA0o">Interna</ref> kaj
          Sud-<ref tip="malprt" cel="azi.OrientaA0o">Orienta Azio</ref>.
	</dif>
      </snc>
      <trd lng="en">China</trd>
      <trd lng="ru">&c_K;&c_i;&c_t;&c_a;&c_j;</trd>
    </drv>

    <drv mrk="cxin.OrientaCx0aMaro">
      <kap>Orienta <tld lit="&Ccirc;"/>a Maro</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
	<dif>
	  <ref tip="malprt" cel="pacifik.0o">Pacifika</ref> maro
          oriente de la kontinenta <tld lit="&Ccirc;"/>ujo.
	</dif>
      </snc>
      <trd lng="en">East China Sea</trd>
      <trd lng="ru">&c_V;&c_o;&c_s;&c_t;&c_o;&c_ch;&c_n;&c_o;-&c_K;&c_i;&c_t;&c_a;&c_j;&c_s;&c_k;&c_o;&c_je; &c_m;&c_o;&c_r;&c_je;</trd>
    </drv>

  </art>

<!--
$Log$
-->
</vortaro>