<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>demisi</rad>/i
    </kap>

    <drv mrk="demisi.0i">
      <kap><tld/>i</kap>
      <snc>
	<dif>
	  Eksofici&gcirc;i, prezenti peton a&ubreve; deklaron per kiu oni rezignas
	  pri sia posteno a&ubreve; ofico:
	  <ekz>
	    ministro <tld/>is pro skandalo
	    <fnt><aut>Roland Rotsaert</aut>:
	      <vrk>Dioksino-krizo</vrk>,
	      <bib>Monato</bib><lok>jaro 1999a, numero 7a, p. 12a</lok></fnt>.
	  </ekz>
	</dif>
	<ref tip="super" cel="eks.0igxi">eksi&gcirc;i</ref>
      </snc>
      <trdgrp lng="be">
        <trd>&c_p;&c_a;&c_j;&c_s;&c_mol;&c_c;&c_ib; &c_w; <ind>&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_k;&c_u;</ind></trd>,
        <trd>&c_s;&c_y;&c_s;&c_mol;&c_c;&c_ib; &c_w; <ind>&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_k;&c_u;</ind></trd>
      </trdgrp>
      <trdgrp lng="de">
	<trd>zur&uuml;cktreten</trd>,
	<trd>sein <ind>Amt</ind> <ind>niederlegen</ind></trd>,
	<trd>demissionieren</trd>
      </trdgrp>
      <trdgrp lng="fr">
	<trd>d&eacute;missionner</trd>,
	<trd>se <ind>d&eacute;mettre</ind></trd>
      </trdgrp>
      <trdgrp lng="hu">
	<trd>lemond<klr tip="ind"> (hivatalr&oacute;l)</klr></trd>,
	<trd>beny&uacute;jtja a <ind>lemond&aacute;s&aacute;t</ind></trd>
      </trdgrp>
      <trd lng="ru">&c_v;&c_y;&c_j;&c_t;&c_i; &c_v; &c_o;&c_t;&c_s;&c_t;&c_a;&c_v;&c_k;&c_u;</trd>
    </drv>

    <drv mrk="demisi.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">POL</uzo>
      <snc mrk="demisi.0o.peto">
	<dif>
	  Rezigna deklaro a&ubreve; peto:
	  <ekz>
	    rifuzi la <tld/>on de policestro, de sekretario
	    <fnt><bib>PIV2</bib></fnt>.
	  </ekz>
	</dif>
      </snc>
      <snc mrk="demisi.0o.eksoficigo">
	<dif>
	  Definitiva maldungo a&ubreve; eksoficigo:
	  <ekz>
	    pluraj procesoj kondamnis urbestrojn a&ubreve; kunlaborantojn [...];
	    okazis multaj <tld/>oj
	    <fnt><aut>Adrien Durandard</aut>: <vrk>FN-fono</vrk>,
	      <bib>Monato</bib><lok>jaro 2002a, numero 7a, p. 12a</lok></fnt>.
	  </ekz>
	</dif>
      </snc>
      <rim>
	Komparon de la sinonimoj vd sub
	<ref cel="abdik.0o.RIM">abdiki</ref>.
      </rim>
      <trd lng="be">&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_k;&c_a;</trd>
      <trdgrp lng="fr">
	<trd>d&eacute;mission</trd>
      </trdgrp>
      <trd lng="hu">lemond&aacute;s<klr tip="ind"> (hivatalr&oacute;l)</klr></trd>
      <trd lng="ru">&c_o;&c_t;&c_s;&c_t;&c_a;&c_v;&c_k;&c_a;</trd>
    </drv>
  </art>
