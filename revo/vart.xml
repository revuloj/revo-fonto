<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vart</rad>/i
</kap>
<drv mrk="vart.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Gardi kaj zorgi estulon, kiu bezonas helpon pro sia juneco:
      <ekz>
        <tld/>i infanon, hundeton;
      </ekz>
      <ekz>
        la infano priver&scirc;adis sian floron kaj <tld/>adis &gcirc;in<fnt>Z</fnt>;
      </ekz>
      <ekz>
        per tro multa <tld/>o <klr>(dorlotado)</klr> malboni&gcirc;as la farto
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fleg.0i">flegi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_n;&c_mol;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_g;&c_l;&c_ja;&c_d;&c_a;&c_c;&c_mol;
      <klr>(&c_d;&c_z;&c_ib;&c_c;&c_ja;)</klr></trd>
  </trdgrp>
  <trd lng="de">h&uuml;ten</trd>
  <trd lng="en">look after</trd>
  <trdgrp lng="fr">
    <trd>prendre soin<klr> (d'un enfant, d'un jeune animal,
    etc.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gondoz</trd>,
    <trd>&aacute;pol</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>oppassen<klr tip="ind"> (v.kind, jong dier of plant)</klr></trd>,
    <trd>verzorgen</trd>
  </trdgrp>
  <trd lng="pt">cuidar de</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_ja;&c_n;&c_ch;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_h;&c_a;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol; <klr tip="amb">(&c_z;&c_a; &c_r;&c_je;&c_b;&c_jo;&c_n;&c_k;&c_o;&c_m; &c_i; &c_t;.&c_p;.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="vart.0istino">
  <kap><tld/>istino</kap>
  <snc>
    <dif>
      Virino, kiu metie <tld/>as infanojn.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="infan.0istino">infanistino</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_n;&c_mol;&c_k;&c_a;</trd>,
    <trd>&c_n;&c_ja;&c_n;&c_je;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_h;&c_a;&c_v;&c_a;&c_w;&c_n;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>nounou</trd>,
    <trd>nourrice</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kinderg&auml;rtnerin</trd>
    <trd>Erzieherin</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>dajka</trd>,
    <trd>dadus</trd>
    <trd>gondoz&oacute;n&odblac;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kindermeisje</trd>,
    <trd>oppas</trd>
  </trdgrp>
  <trd lng="pt">bab&aacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_ja;&c_n;&c_mol;&c_k;&c_a;</trd>,
    <trd>&c_n;&c_ja;&c_n;&c_je;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_o;&c_s;&c_p;&c_i;&c_t;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_c;&c_a; <klr tip="amb">(&c_v; &c_ja;&c_s;&c_l;&c_ja;&c_h;, &c_d;&c_je;&c_t;&c_s;&c_k;&c_o;&c_m; &c_s;&c_a;&c_d;&c_u;)</klr></trd>
  </trdgrp>
</drv>
</art>
