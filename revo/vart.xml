<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vart</rad>/i
</kap>
<drv mrk="vart.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Gardi kaj zorgi estulon, kiu bezonas helpon pro sia juneco:
      <ekz>
        <tld/>i infanon, hundeton;
      </ekz>
      <ekz>
        la infano priver&scirc;adis sian floron kaj <tld/>adis &gcirc;in<fnt>Z</fnt>;
      </ekz>
      <ekz>per tro multa <tld/>o (dorlotado) malboni&gcirc;as la farto
<fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fleg.0i">flegi</ref>.
    </refgrp>
  </snc>
  <trd lng="de">h&uuml;ten</trd>
  <trd lng="en">look after</trd>
  <trdgrp lng="fr">
    <trd>prendre soin<klr> (d'un enfant, d'un jeune animal, etc.)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>oppassen<klr tip="ind"> (v.kind, jong dier of plant)</klr></trd>,
    <trd>verzorgen</trd>
  </trdgrp>
</drv>

<drv mrk="vart.0istino">
  <kap><tld/>istino</kap>
  <snc>
    <dif>
      Virino, kiu metie <tld/>as infanojn.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="infan.0istino">infanistino</ref>
  </refgrp>
  <trdgrp lng="fr">
    <trd>nounou</trd>,
    <trd>nourrice</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kindermeisje</trd>,
    <trd>oppas</trd>
  </trdgrp>
</drv>
</art>
