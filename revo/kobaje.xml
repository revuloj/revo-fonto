   <var><kap><rad var="v">kavied</rad>/oj <fnt><bib>PIV2</bib></fnt></kap></var>
</kap>
<drv mrk="kobaje.0oj">
  <kap><tld/>oj,
     <var><kap><tld var="v"/>oj</kap></var>
  </kap>
  <snc>
     <uzo tip="fak">ZOO</uzo>
     <dif>
       <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
       <ref tip="malprt" cel="rongx.0uloj">ron&gcirc;uloj</ref>
       (<trd lng="la">Caviidae</trd>):
      <ekz>
        bestoj trovi&gcirc;antaj en la zoo: <klr>[&#x2026;]</klr>, kastoroj, <tld var="v"/>oj, kunikloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=485513">Zoologia &gcirc;ardeno de
          Neum&uuml;nster</url></lok>
        </fnt>.
      </ekz>
     </dif>
     <ref tip="sub" cel="kobaj.0o">kobajo</ref>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_y;&c_z;&c_u;&c_h;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#x2019;&c_ja;)</klr></trd>
  <trd lng="ca">c&agrave;vids</trd>
  <trd lng="de">Meerschweinchenartige</trd>
  <trd lng="en">Caviidae <klr>(family)</klr></trd>
  <trd lng="fr">caviid&eacute;s</trd>
  <trd lng="pl">kawiowate</trd>
  <trd lng="ro">caviidae</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_v;&c_ib;&c_jeu;&c_v;&c_ib;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_s;&c_mol;&c_k;&c_ib;/&c_z;&c_a;&c_m;&c_o;&c_r;&c_s;&c_mol;&c_k;&c_ib; &c_s;&c_v;&c_i;&c_n;&c_k;&c_i;</trd>
  </trdgrp>
</drv>
