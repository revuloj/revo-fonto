<art mrk="$Id: kobaje.xml,v 1.9 2022/03/21 17:51:58 revo Exp $">
<kap>
   <rad>kobajed</rad>/oj,
   <var><kap><rad var="v">kavied</rad>/oj</kap></var>
</kap>
<drv mrk="kobaje.0oj">
   <kap><tld/>oj,
     <var><kap><tld var="v"/>oj</kap></var>
   </kap>
   <snc>
     <uzo tip="fak">ZOO</uzo>
     <dif>
       <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
       <ref tip="malprt" cel="rongx.0uloj">ron&gcirc;uloj</ref>
       (<trd lng="la">Caviidae</trd>):
      <ekz>
        bestoj trovi&gcirc;antaj en la zoo: <klr>[&#x2026;]</klr>, kastoroj, <tld var="v"/>oj, kunikloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=485513">Zoologia &gcirc;ardeno de Neum&uuml;nster</url></lok>
        </fnt>.
      </ekz>
     </dif>
     <ref tip="sub" cel="kobaj.0o">kobajo</ref>
   </snc>
   <trd lng="ca">c&agrave;vids</trd>
   <trd lng="de">Meerschweinchenartige</trd>
   <trd lng="fr">caviid&eacute;s</trd>
   <trd lng="pl">kawiowate</trd>     
   <trd lng="ro">caviidae</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x430;&#x432;&#x456;&#x454;&#x432;&#x456;</trd>,
    <trd>&#x43C;&#x43E;&#x440;&#x441;&#x44C;&#x43A;&#x456;/&#x437;&#x430;&#x43C;&#x43E;&#x440;&#x441;&#x44C;&#x43A;&#x456; &#x441;&#x432;&#x438;&#x43D;&#x43A;&#x438;</trd>
  </trdgrp>
</drv>
</art>
