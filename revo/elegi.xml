<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>elegi</rad>/o
</kap>
<drv mrk="elegi.0o">
<kap><tld/>o</kap>
<snc>
<dif>
Malgranda lirika poemo, kun melankolia a&ubreve; sopira karaktero. 
</dif>
</snc>
<trd lng="ru">&c_e;&c_l;&c_je;&c_g;&c_i;&c_ja;</trd>
</drv>
<drv mrk="elegi.0a">
<kap><tld/>a</kap>
<snc num="1">
<dif>
Rilatanta al <tld/>o: <ekz><tld/>a poezio.</ekz>
</dif>
<trd lng="ru">&c_e;&c_l;&c_je;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</snc>
<snc num="2">
<dif>
Plorema kiel <tld/>o: <ekz><tld/>a tono.</ekz>
</dif>
<trdgrp lng="ru">
  <trd>&c_g;&c_r;&c_u;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>,
  <trd>&c_m;&c_je;&c_l;&c_a;&c_n;&c_h;&c_o;&c_l;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
  <trd>&c_u;&c_n;&c_y;&c_l;&c_y;&c_j;</trd>
</trdgrp>
</snc>
</drv>
</art>
