<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: elegi.xml,v 1.27 2025/03/31 22:30:51 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>elegi</rad>/o
<fnt><bib>JED</bib></fnt></kap>
<drv mrk="elegi.0o">
  <kap><tld/>o</kap>
  <snc mrk="elegi.0o.POE">
    <uzo tip="fak">BELE</uzo>
    <uzo tip="fak">POE</uzo>
    <dif>
      Malgranda lirika <ref tip="super" cel="poem.0o.POE">poemo</ref>,
      kun <ref cel="melank.0o">melankolia</ref> a&ubreve;
      <ref cel="sopir.0o.sento">sopira</ref> karaktero:
      <ekz>
        la kanto &scirc;an&gcirc;adis sin iom post iom en plendan, dolorplenan <tld/>on
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la lingvo ta&ubreve;gas plene por &ccirc;iuj poeziaj taskoj: epopeo, satiro, liriko,
        didaktika versa eseo, epigramo, medita&jcirc;o, <tld/>o, poezia dramo, psikologia
        monologo, kabareda kanzono
        <fnt>
          <bib>Plumamikoj</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="ca">elegia</trd>
  <trdgrp lng="cs">
    <trd>duma</trd>,
    <trd>dumka</trd>,
    <trd>elegie</trd>,
    <trd>&zcaron;alozp&ecaron;v</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Elegie</trd>,
    <trd>Klagelied</trd>
  </trdgrp>
  <trd lng="en">elegy</trd>
  <trd lng="es">eleg&iacute;a</trd>
  <trd lng="fr">&eacute;l&eacute;gie</trd>
  <trd lng="hu">el&eacute;gia</trd>
  <trdgrp lng="ja">
    <trd>&#x54C0;&#x6B4C;  <pr>&#12354;&#12356;&#x304B;</pr></trd>,
    <trd>&#x60B2;&#x6B4C;  <pr>&#12402;&#x304B;</pr></trd>,
    <trd>&#x633D;&#x6B4C;  <pr>&#12400;&#12435;&#x304B;</pr></trd>,
    <trd>&#x30A8;&#x30EC;&#x30B8;&#x30FC;</trd>
  </trdgrp>
  <trd lng="pl">elegia</trd>
  <trd lng="pt">elegia</trd>
  <trd lng="ro">elegie</trd>
  <trd lng="ru">&c_e;&c_l;&c_je;&c_g;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>el&eacute;gia</trd>,
    <trd>&zcaron;alospev</trd>
  </trdgrp>
  <trd lng="uk">&c_je;&#x43B;&c_je;&c_g;&c_ib;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x54C0;&#x6B4C; <pr>&amacron;ig&emacron;</pr></trd>,
    <trd>&#x54C0;&#x8F9E; <pr>&amacron;ic&#xED;</pr></trd>,
    <trd>&#x633D;&#x6B4C; <pr>w&#x1CE;ng&emacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="elegi.0a">
  <kap><tld/>a</kap>
  <snc mrk="elegi.0a.POE">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        Solono verkis siajn poemojn grandparte en <tld/>aj disti&hcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>D-ro Winfried Schumacher</aut>
          <vrk><url ref="&Monato;/2012/010605.php">Filozofio metio ideologio</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">elegisch</trd>
    <trd lng="ru">&c_e;&c_l;&c_je;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
  <snc mrk="elegi.0a.FIG">
    <dif>
      Plorema kiel <tld/>o:
      <ekz>
        la deklamanto <klr>[&#8230;]</klr> kantas kun tamburado <tld/>ajn kantojn pri la
        martira morto de imamo <nom>Husejn</nom>
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#8470; 3 (101)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="nostal.0a">nostalgia</ref>,
<ref cel="melank.0a">melankolia</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>schwerm&uuml;tig</trd>,
      <trd>wehm&uuml;tig</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_g;&c_r;&c_u;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_m;&c_je;&c_l;&c_a;&c_n;&c_h;&c_o;&c_l;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_u;&c_n;&c_y;&c_l;&c_y;&c_j;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#24833;&#x601D; <pr>ch&#xF3;us&#x12B;</pr></trd>,
    <trd>&#x60B2; <pr>b&emacron;i</pr></trd>,
    <trd>&#x5FE7;&#x4F24; <pr>y&#x14D;ush&amacron;ng</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_g;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">eleg&iacute;ac</trd>
  <trdgrp lng="cs">
    <trd>elegick&yacute;</trd>,
    <trd>smutn&yacute;</trd>,
    <trd>tkliv&yacute;</trd>,
    <trd>&zcaron;alostiv&yacute;</trd>
  </trdgrp>
  <trd lng="en">elegaic</trd>
  <trd lng="fr">&eacute;l&eacute;giaque</trd>
  <trdgrp lng="he">
    <trd>&ayin;&tsadi;&vav;&bet;</trd>,
    <trd>&qof;&yod;&nun;&tav;&yod;</trd>
  </trdgrp>
  <trd lng="hu">el&eacute;gikus</trd>
  <trdgrp lng="ja">
    <trd>&#x54C0;&#x6B4C;&#x306E;  <pr>&#12354;&#12356;&#x304B;&#x306E;</pr></trd>,
    <trd>&#x60B2;&#x6B4C;&#x306E;  <pr>&#12402;&#x304B;&#x306E;</pr></trd>,
    <trd>&#x54C0;&#24833;&#12434;&#x304A;&#12403;&#x305F;  <pr>&#12354;&#12356;&#12375;&#12421;&#12358;&#12434;&#x304A;&#12403;&#x305F;</pr></trd>,
    <trd>&#29289;&#x60B2;&#12375;&#12356;  <pr>&#12418;&#x306E;&#x304C;&#x306A;&#12375;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="pl">elegijny </trd>
  <trdgrp lng="sk">
    <trd>elegick&yacute;</trd>,
    <trd>smutn&yacute;</trd>,
    <trd>tr&uacute;chliv&yacute;</trd>,
    <trd>&zcaron;ia&lcaron;ny</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_je;&#x43B;&c_je;&c_g;&c_ib;&c_j;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43C;&c_je;&#x43B;&c_a;&#x43D;&c_h;&#x43E;&#x43B;&c_ib;&c_j;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_t;&c_u;&c_zh;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_u;&#x43C;&#x43E;&c_v;&c_i;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_zh;&c_u;&c_r;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
