<art mrk="$Id: pamfle.xml,v 1.20 2019/06/30 18:21:48 revo Exp $">
<kap>
  <ofc>3</ofc><rad>pamflet</rad>/o
</kap>
<drv mrk="pamfle.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELE</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="paskvi.0o">Paskvilo</ref>:
      <ekz>
        liaj maltrankvila&jcirc;oj speguli&gcirc;as en lia lasta poemo, fakte politika pamfleto
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="https://www.monato.be/2003/007925.html">Voja&gcirc;impresoj, somero 2002: Kosovo anta&ubreve;eniras</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la nova&jcirc;oj de la publikaj valenciaj kanaloj fari&gcirc;is &scirc;okaj registaraj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2011/010471.php">Koruptado demisiigas prezidanton</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_m;&c_f;&c_l;&c_je;&c_t;</trd>
  <trd lng="bg">&c_p;&c_a;&c_m;&c_f;&c_l;&c_je;&c_t;</trd>
  <trdgrp lng="ca">
    <trd>pamflet</trd>,
    <trd>libel</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hanopis</trd>,
    <trd>pamflet</trd>
  </trdgrp>
  <trd lng="de">Pamphlet</trd>
  <trdgrp lng="en">
    <trd>pasquinade</trd>,
    <trd>lampoon</trd>,
    <trd>satire</trd>,
    <trd>pamphlet
      <klr tip="amb">(harsh essay about a current topic)</klr>
    </trd>
  </trdgrp>
  <trd lng="es">panfleto</trd>
  <trdgrp lng="fr">
    <trd>libelle</trd>,
    <trd>pamphlet</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>pamflet</trd>,
    <trd>g&uacute;nyirat</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>libello</trd>,
    <trd>pamphlet</trd>
  </trdgrp>
  <trd lng="nl">pamflet</trd>
  <trd lng="pl">pamflet</trd>
  <trd lng="pt">panfleto</trd>
  <trd lng="ru">&c_p;&c_a;&c_m;&c_f;&c_l;&c_je;&c_t;</trd>
  <trd lng="sk">pamflet</trd>
</drv>
</art>
