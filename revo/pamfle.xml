<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc><rad>pamflet</rad>/o
</kap>
<drv mrk="pamfle.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELE</uzo>
  <snc>
    <ref tip="dif" cel="paskvi.0o">paskvilo</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_m;&c_f;&c_l;&c_je;&c_t;</trd>
  <trd lng="bg">&c_p;&c_a;&c_m;&c_f;&c_l;&c_je;&c_t;</trd>
  <trdgrp lng="ca">
    <trd>pamflet</trd>,
    <trd>libel</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>pasquinade</trd>,
    <trd>lampoon</trd>,
    <trd>satire</trd>,
    <trd>pamphlet
      <klr tip="amb">(harsh essay about a current topic)</klr>
    </trd>
  </trdgrp>
  <trd lng="de">Pamphlet</trd>
  <trd lng="es">panfleto</trd>
  <trdgrp lng="fr">
    <trd>libelle</trd>,
    <trd>pamphlet</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>pamflet</trd>,
    <trd>g&uacute;nyirat</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>libello</trd>,
    <trd>pamphlet</trd>
  </trdgrp>
  <trd lng="nl">pamflet</trd>
  <trd lng="pl">pamflet</trd>
  <trd lng="pt">panfleto</trd>
  <trd lng="ru">&c_p;&c_a;&c_m;&c_f;&c_l;&c_je;&c_t;</trd>
</drv>
</art>
