<art mrk="$Id: destrojer.xml,v 1.12 2023/11/04 13:16:27 revo Exp $">
<kap>
  <rad>destrojer</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="destrojer.0o">
  <kap><tld/>o</kap>
  <snc mrk="destrojer.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Mezgranda milit&scirc;ipo kapabla defendi aliajn &scirc;ipojn
      kontra&ubreve; 
      &ccirc;iaspecaj atakoj:
      <ekz>
        torped&scirc;ipoj kaj <tld/>oj
        <fnt>
        <bib>Salamandroj</bib>,
        <lok>libro 3a, &ccirc;apitro 3a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj restis &scirc;ipbordo al &scirc;ipbordo, po kvar
        &scirc;ipoj kune
        <fnt>
        <aut>Ilja Ilf, Evgenij Petrov, trad. V. Okc</aut>,
        <vrk><url ref="http://www.esperanto.mv.ru/Usono/Enhavo.html">Unueta&gcirc;a
        Usono</url></vrk>,
        <lok>&ccirc;apitro 40a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ib;&c_n;&c_a;&c_n;&c_o;&c_s;&c_je;&c_c;</trd>,
    <trd>&c_e;&c_s;&c_mol;&c_m;&c_ib;&c_n;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="cs">torp&#xE9;dovka</trd>
  <trd lng="de">Zerst&ouml;rer<klr tip="amb"> (Schiff)</klr></trd>
  <trdgrp lng="fr">
    <trd>destroyer</trd>,
    <trd>fr&eacute;gate<klr> (destroyer)</klr></trd>
  </trdgrp>
  <trd lng="he">&#x5DE;&#x5B7;&#x5E9;&#x5C1;&#x5D7;&#x5B6;&#x5EA;&#x5B6;&#x5EA;</trd>
  <trd lng="it">cacciatorpediniere</trd>
  <trd lng="pl">niszczyciel</trd>
  <trdgrp lng="pt">
    <trd>contratorpedeiro</trd>,
    <trd>destr&oacute;ier</trd>
  </trdgrp>
  <trd lng="sk">torp&#xE9;dovka</trd>
  <trd lng="sv">jagare<klr tip="amb"> (fartyg)</klr></trd>
  <trd lng="uk">&#x43C;&c_ib;&#x43D;&#x43E;&#x43D;&#x43E;&c_s;&c_je;&c_c;&#x44C;</trd>
  <trdgrp lng="zh">
    <trd>&#x9A45;&#x8FFD;&#x8266; <pr>q&#x16B;zhu&#x12B;ji&#xE0;n</pr></trd>,
    <trd>&#x9A45;&#x9010;&#x8266; <pr>q&#x16B;zh&#xFA;ji&#xE0;n</pr></trd>
  </trdgrp>
</drv>
</art>

