<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>destrojer</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="destrojer.0o">
  <kap><tld/>o</kap>
  <snc mrk="destrojer.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Mezgranda milit&scirc;ipo kapabla defendi aliajn &scirc;ipojn
      kontra&ubreve; 
      &ccirc;iaspecaj atakoj:
      <ekz>
        torped&scirc;ipoj kaj <tld/>oj
        <fnt>
        <bib>Salamandroj</bib>,
        <lok>libro 3a, &ccirc;apitro 3a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj restis &scirc;ipbordo al &scirc;ipbordo, po kvar
        &scirc;ipoj kune
        <fnt>
        <aut>Ilja Ilf, Evgenij Petrov, trad. V. Okc</aut>,
        <vrk><url
        ref="http://www.esperanto.mv.ru/Usono/Enhavo.html">Unueta&gcirc;a
        Usono</url></vrk>,
        <lok>&ccirc;apitro 40a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Zerst&ouml;rer<klr tip="amb"> (Schiff)</klr></trd>
  <trdgrp lng="fr">
    <trd>destroyer</trd>,
    <trd>fr&eacute;gate<klr> (destroyer)</klr></trd>
  </trdgrp>
  <trd lng="pl">niszczyciel</trd>
  <trdgrp lng="pt">
    <trd>contratorpedeiro</trd>,
    <trd>destr&oacute;ier</trd>
  </trdgrp>
</drv>
</art>

