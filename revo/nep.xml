<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>nep</rad>/o
</kap>

<drv mrk="nep.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">GEN</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="fil.0o" lst="voko:parencoj">Filo</ref> de la
      filo a&ubreve; filino rilate al ilia patro kaj patrino:
      <ekz>
        <tld/>oj estas krono de maljunulinoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        de antikveco avino amegas sian <tld/>on
        <fnt><bib>RugxDom</bib>
        <lok>&ccirc;apitro 56a, p. 250a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_n;&c_u;&c_k;</trd>
  <trd lng="br">mab-bihan</trd>
  <trd lng="cs">vnuk</trd>
  <trd lng="de">Enkel</trd>
  <trd lng="el">
  &epsilon;&gamma;&gamma;&omikron;&ny;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">grandson</trd>
  <trd lng="es">nieto</trd>
  <trd lng="fr">petit-fils</trd>
  <trd lng="he">&nun;&kaf;&dalet;</trd>
  <trd lng="hu">unoka</trd>
  <trd lng="nl">kleinzoon</trd>
  <trd lng="pl">wnuk</trd>
  <trd lng="pt">neto</trd>
  <trd lng="ru">&c_v;&c_n;&c_u;&c_k;</trd>
  <trdgrp lng="sv">
    <trd>barnbarn<klr> (pojke)</klr></trd>,
    <trd>sonson</trd>,
    <trd>dotterson</trd>
  </trdgrp>
  <trd lng="tr">erkek torun</trd>
</drv>

<drv mrk="nep.0ino">
  <kap><tld/>ino</kap>
  <uzo tip="fak">GEN</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="fil.0ino" lst="voko:parencoj">Filino</ref> de
      la filo a&ubreve; filino rilate al ilia patro kaj
      patrino:
      <ekz>la tri propraj <tld/>inoj ricevis malpli da zorgo en la
      &ccirc;iutaga vivo
      <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 5a, p. 63a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_n;&c_u;&c_ch;&c_k;&c_a;</trd>
  <trd lng="br">merc'h-vihan</trd>
  <trd lng="de">Enkelin</trd>
  <trd lng="en">granddaughter</trd>
  <trd lng="fr">petite-fille</trd>
  <trd lng="nl">kleindochter</trd>
  <trd lng="pl">wnuczka</trd>
  <trd lng="pt">neta</trd>
  <trd lng="ru">&c_v;&c_n;&c_u;&c_ch;&c_k;&c_a;</trd>
  <trdgrp lng="sv">
    <trd>barnbarn<klr> (flicka)</klr></trd>,
    <trd>sondotter</trd>,
    <trd>dotterdotter</trd>
  </trdgrp>
  <trd lng="tr">k&inodot;z torun</trd>
</drv>

<drv mrk="nep.pra0opost0o">
  <kap>pra<tld/>o, <var><kap>post<tld/>o</kap></var></kap>
  <uzo tip="fak">GEN</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="fil.0o" lst="voko:parencoj">Filo</ref> de
      <tld/>o a&ubreve; <tld/>ino:
      <ekz>
        lia bonfaremeco iras &gcirc;is la pra<tld/>oj<fnt><bib>MT</bib>
        <lok>&Psa;
        103:17</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_w;&c_n;&c_u;&c_k;</trd>
  <trd lng="br">mab-ku&ntilde;v</trd>
  <trd lng="de">Urenkel</trd>
  <trd
  lng="el">&delta;&jota;&sigma;&epsilon_ton;&gamma;&gamma;&omikron;&ny;&omikron;&sigma_fina;</trd>
  <trd lng="en">great-grandson</trd>
  <trd lng="fr">arri&egrave;re-petit-fils</trd>
  <trd lng="he">&nun;&yod;&fnun;</trd>
  <trd lng="hu">d&eacute;dunoka</trd>
  <trd lng="nl">achterkleinzoon</trd>
  <trd lng="pl">prawnuk</trd>
  <trd lng="pt">bisneto</trd>
  <trd lng="ru">&c_p;&c_r;&c_a;&c_v;&c_n;&c_u;&c_k;</trd>
  <trdgrp lng="sv">
    <trd>barnbarnsbarn<klr> (pojke)</klr></trd>,
    <trd>barnbarnsson</trd>
  </trdgrp>
</drv>

<drv mrk="nep.pra0opost0ino">
  <kap>pra<tld/>ino, <var><kap>post<tld/>ino</kap></var></kap>
  <uzo tip="fak">GEN</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="fil.0ino" lst="voko:parencoj">Filino</ref> de
      <tld/>o a&ubreve; <tld/>ino.
    </dif>
  </snc>
  <trd lng="nl">achterkleindochter</trd>
  <trd lng="pl">prawnuczka</trd>
  <trdgrp lng="sv">
    <trd>barnbarnsbarn<klr> (flicka)</klr></trd>,
    <trd>barnbarnsdotter</trd>
  </trdgrp>
</drv>
</art>
