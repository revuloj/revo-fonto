<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nep.xml,v 1.40 2021/05/28 12:15:12 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>nep</rad>/o
</kap>

<drv mrk="nep.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">GEN</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="fil.0o" lst="voko:parencoj">Filo</ref> de la
      filo a&ubreve; filino rilate al ties gepatroj:
      <ekz>
        <tld/>oj estas krono de maljunulinoj
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 17:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ion &ccirc;i tion <klr>[&#x2026;]</klr> iam rakontis maljuna avo al sia
        malgranda <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Dano Holger</lok>
        </fnt>;
      </ekz>
      <ekz>
        de antikveco avino amegas sian <tld/>on
        <fnt><bib>RugxDom</bib>
        <lok>&ccirc;apitro 56a, p. 250a</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>oj vin benos, se vi pacience eltenos
        <fnt>
          <aut>L. L. Zamenhof</aut>
          <vrk>La Vojo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        li estas preska&ubreve; mia frato: <tld/>o de kuzo de onklo de konato
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        nia e&ubreve;ropa civilizacio, <tld/>o de la gloraj kulturoj de Grekio
        kaj Romo, ido de
        Renesanco, lulilo de grandaj pentristoj, poetoj, komponistoj,
        filozofoj
        <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007076.html">Ho, tempoj! Ho, moroj!</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_n;&c_u;&c_k;</trd>
  <trd lng="bo">&#xF5A;&#xF0B;&#xF56;&#xF7C;&#xF0B;</trd>
  <trd lng="br">mab-bihan</trd>
  <trd lng="cs">vnuk</trd>
  <trd lng="de">Enkel</trd>
  <trd lng="el">&epsilon;&gamma;&gamma;&omikron;&ny;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">grandson</trd>
  <trd lng="es">nieto</trd>
  <trd lng="fa">&a_n;&a_w;&a_h;<klr tip="ind"> (&f_p;&a_s;&a_r;)</klr></trd>
  <trd lng="fr">petit-fils</trd>
  <trd lng="he">&nun;&kaf;&dalet;</trd>
  <trd lng="hu">unoka</trd>
  <trd lng="id">cucu</trd>
  <trd lng="io">nepotulo</trd>
  <trd lng="nl">kleinzoon</trd>
  <trd lng="pl">wnuk</trd>
  <trd lng="pt">neto</trd>
  <trd lng="ru">&c_v;&c_n;&c_u;&c_k;</trd>
  <trd lng="sk">vnuk</trd>
  <trdgrp lng="sv">
    <trd>barnbarn<klr> (pojke)</klr></trd>,
    <trd>sonson</trd>,
    <trd>dotterson</trd>
  </trdgrp>
  <trd lng="tr">erkek torun</trd>
  <trd lng="vo">posson</trd>
</drv>

<drv mrk="nep.0ino">
  <kap><ofc>*</ofc><tld/>ino</kap>
  <uzo tip="fak">GEN</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="fil.0ino" lst="voko:parencoj">Filino</ref> de
      la filo a&ubreve; filino rilate al ties gepatroj:
      <ekz>
        mi vidis vian avinon kun &scirc;iaj kvar <tld/>inoj kaj kun mia
        nevino
      <fnt><bib>F</bib>, <lok>&FE; 33</lok></fnt>
      </ekz>
      <ekz>
        Siajn filojn kaj <tld/>ojn, siajn filinojn kaj <tld/>inojn, kaj
        sian tutan
        idaron li venigis kun si
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 46:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tri propraj <tld/>inoj ricevis malpli da zorgo en la
        &ccirc;iutaga vivo
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 5a, p. 63a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_n;&c_u;&c_ch;&c_k;&c_a;</trd>
  <trd lng="bo">&#xF5A;&#xF0B;&#xF58;&#xF7C;&#xF0B;</trd>
  <trd lng="br">merc'h-vihan</trd>
  <trd lng="cs">vnu&ccaron;ka</trd>
  <trd lng="de">Enkelin</trd>
  <trd lng="en">granddaughter</trd>
  <trd lng="es">nieta</trd>
  <trd lng="fa">&a_n;&a_w;&a_h;<klr tip="ind"> (&a_d;&a_kh;&a_t;&a_r;)</klr></trd>
  <trd lng="fr">petite-fille</trd>
  <trd lng="hu">unoka</trd>
  <trd lng="id">cucu</trd>
  <trd lng="io">nepotino</trd>
  <trd lng="nl">kleindochter</trd>
  <trd lng="pl">wnuczka</trd>
  <trd lng="pt">neta</trd>
  <trd lng="ru">&c_v;&c_n;&c_u;&c_ch;&c_k;&c_a;</trd>
  <trd lng="sk">vnu&ccaron;ka</trd>
  <trdgrp lng="sv">
    <trd>barnbarn<klr> (flicka)</klr></trd>,
    <trd>sondotter</trd>,
    <trd>dotterdotter</trd>
  </trdgrp>
  <trd lng="tr">k&inodot;z torun</trd>
  <trd lng="vo">posdaut</trd>
</drv>

<drv mrk="nep.ge0oj">
  <kap>ge<tld/>oj</kap>
  <snc>
    <dif>
      <tld lit="N"/>o(j) kaj <tld/>ino(j):
      <ekz>
        &scirc;i timas <klr>[&#x2026;]</klr> enfali en mizeron kune kun siaj infanoj
        kaj ge<tld/>oj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi estas pensiulo <klr>[&#x2026;]</klr> kun du filinoj kaj kvin
        ge<tld/>oj (inter ili
        &gcirc;emelinoj)
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="https://www.monato.be/2003/008009.html">Stefan Maul</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la ministro ne volas komuniki&gcirc;i kun siaj gefiloj kaj ge<tld/>oj
        kaj ilin e&ccirc; ne
        agnoskis
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="https://www.monato.net/2005/008564.php">Ka&scirc;ita
          kulturo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Enkelkinder</trd>
  <trd lng="en">grandchildren</trd>
  <trd lng="fr">petits-enfants</trd>
  <trd lng="io">genepoti</trd>
  <trd lng="nl">kleinkinderen</trd>
</drv>

<drv mrk="nep.ge0o">
  <kap>ge<tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="nep.ge0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="lst" cel="id.0o.GEN" lst="voko:parencoj">Ido</ref> de la
      filo a&ubreve; filino rilate al ties gepatroj:
      <ekz>
        kompreneble li ne deziris, ke lia unua ge<tld/>o naski&gcirc;u sklavo
        <fnt>
          <aut>A. L&ouml;wenstein</aut>,
          <vrk>Morto de Artisto</vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      &Ccirc;i tiu uzo devias de la tradicia uzo, kaj ne &ccirc;iuj esperantistoj
      akceptas &gcirc;in. Vidu la <ref cel="ge.0.ununombro">duan sencon de
      ge-</ref>.
    </rim>
  </snc>
  <trd lng="cs">vnou&ccaron;e</trd>
  <trd lng="da">barnebarn</trd>
  <trd lng="de">Enkelkind</trd>
  <trd lng="en">grandchild</trd>
  <trd lng="fa">&a_n;&a_w;&a_h;</trd>
  <trd lng="fr">petit-enfant</trd>
  <trd lng="hu">unoka</trd>
  <trd lng="id">cucu</trd>
  <trd lng="io">nepoto</trd>
  <trd lng="is">barnabarn</trd>
  <trd lng="nl">kleinkind</trd>
  <trd lng="sv">barnbarn</trd>
  <trd lng="sw">mjukuu</trd>
  <trd lng="tr">torun</trd>
  <trd lng="vo">poscil</trd>
</drv>

<drv mrk="nep.pra0opost0o">
  <kap><ofc>*</ofc>pra<tld/>o, <var><kap>post<tld/>o</kap></var></kap>
  <uzo tip="fak">GEN</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="fil.0o" lst="voko:parencoj">Filo</ref> de
      <tld/>o a&ubreve; <tld/>ino:
      <ekz>
        la filoj, <tld/>oj kaj pra<tld/>oj de re&gcirc;o estas re&gcirc;idoj
        <fnt>
          <bib>F</bib>
          <lok>&FE; 36</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia bonfaremeco iras &gcirc;is la pra<tld/>oj<fnt><bib>MT</bib>
        <lok>&Psa;
        103:17</lok></fnt>;
      </ekz>
      <ekz>
        atendu min anta&ubreve; la domo de mia pra<tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        Dio <klr>[&#x2026;]</klr> donu al vi idaron grandnombran, <tld/>ojn kaj
        pra<tld/>ojn
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>!
      </ekz>
      <ekz>
        <nom>Alfred Nobel</nom> estas unu el liaj post<tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=161837">Olof Rudbeck</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_w;&c_n;&c_u;&c_k;</trd>
  <trd lng="br">mab-ku&ntilde;v</trd>
  <trd lng="cs">pravnuk</trd>
  <trd lng="de">Urenkel</trd>
  <trd lng="el">&delta;&jota;&sigma;&epsilon_ton;&gamma;&gamma;&omikron;&ny;&omikron;&sigma_fina;</trd>
  <trd lng="en">great-grandson</trd>
  <trd lng="es">bisnieto</trd>
  <trd lng="fa">&a_n;&a_t;&f_y;&a_j;&a_h;<klr tip="ind"> (&a_f;&a_r;&a_z;&a_n;&a_d; &a_n;&a_w;&a_h;&a_komo; &f_p;&a_s;&a_r;)</klr></trd>
  <trd lng="fr">arri&egrave;re-petit-fils</trd>
  <trd lng="he">&nun;&yod;&fnun;</trd>
  <trd lng="hu">d&eacute;dunoka</trd>
  <trd lng="id">cicit</trd>
  <trd lng="io">posnepotulo</trd>
  <trd lng="nl">achterkleinzoon</trd>
  <trd lng="pl">prawnuk</trd>
  <trd lng="pt">bisneto</trd>
  <trd lng="ru">&c_p;&c_r;&c_a;&c_v;&c_n;&c_u;&c_k;</trd>
  <trd lng="sk">pravnuk</trd>
  <trdgrp lng="sv">
    <trd>barnbarnsbarn<klr> (pojke)</klr></trd>,
    <trd>barnbarnsson</trd>
  </trdgrp>
  <trd lng="vo">posposson</trd>
</drv>

<drv mrk="nep.pra0opost0ino">
  <kap>pra<tld/>ino, <var><kap>post<tld/>ino</kap></var></kap>
  <uzo tip="fak">GEN</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="fil.0ino" lst="voko:parencoj">Filino</ref> de
      <tld/>o a&ubreve; <tld/>ino:
      <ekz>
        la Vilao Muzeo estas nune posedo de <klr>[l]</klr>ia
        pra<tld/>ino
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16507">Giacomo Puccini</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Morris De Ketelaere</nom> <klr>[&#x2026;]</klr> estas la prapatro
        de granda antverpena
        esperantista familio, kun i.a. <klr>[&#x2026;]</klr> <nom>Karlijn Van
        Damme</nom> (pra<tld/>ino)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=32747">Morris De Ketelaere</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">pravnu&ccaron;ka</trd>
  <trd lng="de">Urenkelin</trd>
  <trd lng="en">great-granddaughter</trd>
  <trd lng="es">bisnieta</trd>
  <trd lng="fa">&a_n;&a_t;&f_y;&a_j;&a_h;<klr tip="ind"> (&a_f;&a_r;&a_z;&a_n;&a_d; &a_n;&a_w;&a_h;&a_komo; &a_d;&a_kh;&a_t;&a_r;)</klr></trd>
  <trd lng="hu">d&eacute;dunoka</trd>
  <trd lng="id">cicit</trd>
  <trd lng="io">posnepotino</trd>
  <trd lng="nl">achterkleindochter</trd>
  <trd lng="pl">prawnuczka</trd>
  <trd lng="pt">bisneta</trd>
  <trd lng="sk">pravnu&ccaron;ka</trd>
  <trdgrp lng="sv">
    <trd>barnbarnsbarn<klr> (flicka)</klr></trd>,
    <trd>barnbarnsdotter</trd>
  </trdgrp>
  <trd lng="vo">posposdaut</trd>
</drv>
</art>
