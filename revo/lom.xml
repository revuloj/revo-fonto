<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>lom</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="lom.0o">
  <kap><tld/>o</kap>
  <snc mrk="lom.0o.GRU">
    <uzo tip="fak">KON</uzo>
    <uzo tip="fak">MIN</uzo>
    <dif>
      <ref tip="super" cel="grund.0o.KOMUNE">Grundo</ref>
      duone sabla, duone argila kaj &scirc;lima,
      ordinare fruktodona, anka&ubreve; uzata kiel materialo
      por muroj:
      <ekz>
        ili lo&gcirc;is en malaltaj, <tld/>aj dometoj
        <fnt>
          <aut>S. Lang, trad. G. Berveling</aut>,
          <vrk>La Talpo&ccirc;asisto</vrk>,
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">loam</trd>
</drv>
</art>

