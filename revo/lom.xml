<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lom.xml,v 1.5 2022/03/22 18:26:51 revo Exp $">
<kap>
  <rad>lom</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="lom.0o">
  <kap><tld/>o</kap>
  <snc mrk="lom.0o.GRU">
    <uzo tip="fak">KON</uzo>
    <uzo tip="fak">MIN</uzo>
    <dif>
      <ref tip="super" cel="grund.0o.KOMUNE">Grundo</ref>
      duone sabla, duone argila kaj &scirc;lima,
      ordinare fruktodona, anka&ubreve; uzata kiel materialo
      por muroj:
      <ekz>
        ili lo&gcirc;is en malaltaj, <tld/>aj dometoj
        <fnt>
          <aut>S. Lang, trad. G. Berveling</aut>,
          <vrk>La Talpo&ccirc;asisto</vrk>,
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd><klr>(zem.)</klr> p&uring;da</trd>,
    <trd>hl&iacute;na</trd>,
    <trd>prs&tcaron;</trd>
  </trdgrp>
  <trd lng="fr">loam</trd>
  <trd lng="pl">glina</trd>     
  <trd lng="ro">argil&abreve;</trd>
  <trdgrp lng="sk">
    <trd>hlina</trd>,
    <trd>prs&tcaron;</trd>,
    <trd>p&ocirc;da</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x435;&#x433;&#x43A;&#x438;&#x439; &#x491;&#x440;&#x443;&#x43D;&#x442;</trd>,
    <trd>&#x43B;&#x43E;&#x43C;&#x430;&#x441;</trd>
  </trdgrp>
</drv>
</art>

