    <drv mrk="plant.0o">
      <kap><tld/>o</kap>
      <snc mrk="plant.0o.ado">
        <dif>
          La ago <tld/>i (= <tld/>ado)
          kaj &gcirc;ia rezulto (= <ref tip="sin" cel="plant.0ajxo.AGR"><tld/>a&jcirc;o</ref>):
          <ekz>
            ofte nur la infanoj atingas plenan &gcirc;uon de la
            <tld/>oj, kiujn iliaj patroj faris
            <fnt>
              <bib>FK</bib>,
              <vrk>Kio estas vegetarismo?</vrk>
            </fnt>.
          </ekz>
        </dif>
        <trdgrp lng="be">
          <trd>&c_s;&c_a;&c_d;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
          <trd>&c_p;&c_a;&c_s;&c_a;&c_d;&c_k;&c_a; <klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_y;)</klr></trd>
        </trdgrp>
        <trd lng="en">planting</trd>
        <trd lng="id"><baz>tanam</baz> penanaman</trd>
        <trd lng="ru">&c_p;&c_o;&c_s;&c_a;&c_d;&c_k;&c_a;</trd>
        <trd lng="pl">nasadzenie</trd>
        <trdgrp lng="de">
          <trd>Pflanzen</trd>,
          <trd>Pflanzung</trd>
        </trdgrp>
      <trd lng="th">&#xE01;&#xE32;&#xE23;&#xE1B;&#xE25;&#xE39;&#xE01;&#xE1E;&#xE37;&#xE0A;</trd>
      </snc>
      <snc mrk="plant.0o.vegetajxo">
        <uzo tip="fak">BOT</uzo>
        <refgrp tip="dif">
          <ref cel="veget.0ajxo">vegeta&jcirc;o</ref>,
          <ref cel="vegeta.0o">vegetalo</ref>,
          <ref cel="kresk.0ajxo.vegetajxo">kreska&jcirc;o<sncref/></ref>
        </refgrp>
        <refgrp tip="malprt">
          <ref cel="vegeta.0aregno">vegetala regno</ref>,
          <ref cel="plant.0oj"><tld/>oj</ref>
        </refgrp>
        <rim>
          &Ccirc;i tiu senco de la substantivo <tld/>o ne estas
          fundamenta,
          oficiala a&ubreve; zamenhofa. Fundamenta estas nur la verba
          senco
          &#xAB;<tld/>i&#xBB;.
          <aut>Sergio Pokrovskij</aut>
        </rim>
        <trd lng="be">&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;</trd>
        <trd lng="de">Pflanze</trd>
        <trd lng="en">plant</trd>
        <trd lng="fr">plante</trd>
        <trd lng="hu">n&ouml;v&eacute;ny</trd>
        <trdgrp lng="id">
          <trd><ind>tanam</ind>an</trd>,
          <trd><ind>tumbuh</ind>an</trd>
        </trdgrp>
        <trd lng="nl">plant</trd>
        <trd lng="pl">ro&sacute;lina</trd>
        <trd lng="ru">&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;</trd>
        <trd lng="th">&#xE1E;&#xE37;&#xE0A;</trd>
        <trd lng="tok">kasi</trd>
      </snc>
  <trd lng="bo">&t_sha;&t_gigu;&t_nga;&t_tsheg;&t_tsha;&t_la;&t_tsheg;</trd>
  <trd lng="cs">rostlina</trd>
  <trd lng="ja">&#x690D;&#x7269;  <pr>&#x3057;&#x3087;&#x304F;&#x3076;&#x3064;</pr></trd>
  <trdgrp lng="sk">
