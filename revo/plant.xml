  <trd lng="bs">postrojenja</trd>
  <trd lng="co">pianta</trd>
  <trdgrp lng="cs">
    <trd>nas&aacute;zet rostliny</trd>,
    <trd>s&aacute;zet rostliny</trd>,
    <trd>vsadit rostliny</trd>,
