<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>plant</rad>/i
</kap>
<drv mrk="plant.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Meti vegeta&jcirc;on en teron por ke &gcirc;i plu kresku:
      <ekz>
        <tld/>i rozujon;
      </ekz>
      <ekz>
        <tld/>i vinber&gcirc;ardenon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i vinberbran&ccirc;ojn en sia &gcirc;ardeno<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>ita <klr>(senmove staranta)</klr> kiel paliso;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i principon en la kapon de infano;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        novaj pensoj ek<tld/>i&gcirc;is (enradiki&gcirc;is) en lia animo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">pflanzen</trd>
  <trd lng="en">plant</trd>
  <trd lng="hu">&uuml;ltet<klr tip="ind"> (n&ouml;v&eacute;nyt)</klr></trd>
  <trd lng="nl">planten</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_a;&c_zh;&c_a;&c_t;&c_mol;<klr tip="ind"> (&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>,
    <trd>&c_p;&c_o;&c_s;&c_a;&c_d;&c_i;&c_t;&c_mol; <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="plant.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="plant.0o.vegetajxo">
    <dif>
       = <ref tip="dif" cel="veget.0ajxo">Vegeta&jcirc;o</ref>.
    </dif>
    <ref tip="malprt" cel="plant.0oj"><tld/>oj</ref>
	<rim>
	  &Ccirc;i tiu senco de la substantivo <tld/>o ne estas fundamenta
	  a&ubreve; oficiala.  Fundamenta estas nur la verba senco &leftquot;<tld/>i&rightquot;.
	  <aut>Sergio Pokrovskij</aut>
	</rim>
  </snc>
  <trd lng="de">Pflanze</trd>
  <trd lng="en">plant</trd>
  <trd lng="hu">n&ouml;v&eacute;ny</trd>
  <trd lng="nl">plant</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="plant.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Unu el la <ref tip="malprt" cel="regn.0o.BIO">regnoj</ref>
      (<trd lng="la">Plantae</trd>) de la viva&jcirc;oj.
    </dif>
  </snc>
  <trd lng="hu">n&ouml;v&eacute;nyek orsz&aacute;ga</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_ja;</trd>,
    <trd>&c_c;&c_a;&c_r;&c_s;&c_t;&c_v;&c_o; &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="plant.0ajxo">
  <kap><tld/>ita&jcirc;o, <tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Juna <tld/>ita a&ubreve; <tld/>ota&ubreve;ga vegeta&jcirc;o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Jehudo estas lia plej amata <tld/>a&jcirc;o<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="hu">pal&aacute;nta</trd>
  <trd lng="ru">&c_s;&c_a;&c_zh;&c_je;&c_n;&c_je;&c_c;</trd>
</drv>

<drv mrk="plant.0ido">
  <kap><tld/>ido</kap>
  <snc>
    <dif>
      Nova trunketo, kiun vegeta&jcirc;o kreskigas el tero:
      <ekz>
        harmonie el la malnova bazo <tld/>idoj novaj &ccirc;arme kreskis
        supren<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Sch&ouml;&szlig;ling</trd>
  <trd lng="hu">sarjhajt&aacute;s</trd>
  <trdgrp lng="nl">
    <trd>scheut</trd>,
    <trd>spruit</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_o;&c_s;&c_t;&c_o;&c_k;</trd>
</drv>

<drv mrk="plant.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Loko multe <tld/>ita.
    </dif>
  </snc>
  <trd lng="de">Plantage</trd>
  <trd lng="hu">&uuml;ltetv&eacute;ny</trd>
  <trdgrp lng="nl">
    <trd>plantage</trd>,
    <trd>aanplanting</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_a;&c_n;&c_t;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_u;&c_ch;&c_a;&c_s;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="plant.en0i">
  <kap>en<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Profunde <tld/>i.
    </dif>
    <trd lng="hu">j&oacute;l <ind>el&uuml;ltet</ind></trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Profunde penetrigi en la spiriton:
      <ekz>
        la sento pri honoro estas eduke en<tld/>ata &ccirc;e la Japanoj;
      </ekz>
      <ekz>
        tiel la lernemo en<tld/>i&gcirc;as &ccirc;e tiuj infanoj.
      </ekz>
    </dif>
    <trd lng="hu">belepl&aacute;nt&aacute;l</trd>
    <trd lng="nl">inprenten</trd>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_v;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_zh;&c_i;&c_v;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_m;&c_p;&c_l;&c_a;&c_n;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="plant.trans0i">
  <kap>trans<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="P"/>i ion en alian lokon.
    </dif>
    <trdgrp lng="nl">
      <trd>transplanteren</trd>,
      <trd>verplanten</trd>,
      <trd>overbrengen</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Penetrigi ideon a&ubreve; kutimon en alian spiriton a&ubreve; medion:
      <ekz>
        tiu supersti&ccirc;o trans<tld/>i&gcirc;is en la najbaran landon;
      </ekz>
      <ekz>
        tiuj radikoj trans<tld/>i&gcirc;is el la franca en la anglan vortaron.
      </ekz>
    </dif>
  </snc>
  <trd lng="hu">&aacute;t&uuml;ltet</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_s;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_l;&c_a;&c_n;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="plant.dauxra0o">
  <kap>da&ubreve;ra <tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="plant.0o">Planto</ref>, floranta
      kaj fruktanta plurfoje dum sia vivo:
      <ekz>
        sta&ubreve;do, arbusto, arbedo kaj arbo estas
        da&ubreve;raj <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="hu">&eacute;vel&odblac; n&ouml;v&eacute;ny</trd>
  <trdgrp lng="de">
    <trd>ausdauernde Pflanze</trd>,
    <trd>perennierende Pflanze</trd>
  </trdgrp>
  <trd lng="en">perennial plant</trd>
  <trd lng="ru">&c_m;&c_n;&c_o;&c_g;&c_o;&c_l;&c_je;&c_t;&c_n;&c_je;&c_je; &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="plant.nedauxra0o">
  <kap>neda&ubreve;ra planto</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="plant.0o">Planto</ref>, vivanta
      nur unu a&ubreve; malmultajn jarojn kaj floranta
      kaj fruktanta nur unufoje dum sia vivo:
      <ekz>
        herbo estas neda&ubreve;ra <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="hu">egyny&aacute;ri n&ouml;v&eacute;ny</trd>
  <trd lng="ru">&c_o;&c_d;&c_n;&c_o;&c_l;&c_je;&c_t;&c_n;&c_je;&c_je; &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>
</art>
