    <drv mrk="plant.en0i">
      <kap>en<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="plant.en0i.KOMUNE">
        <dif>
          Enigi ion (ekzemple semon) en pli grandan korpon a&ubreve; medion
          (ekzemple grundon), por ke &gcirc;i ta&ubreve;ge evoluu a&ubreve; funkciu.
        </dif>
        <subsnc mrk="plant.en0i.HOR">
          <uzo tip="fak">HOR</uzo>
          <dif>
            Enigi en la grundon por kreskigo.
          </dif>
          <trdgrp lng="pl">
            <trd>sia&cacute;</trd>,
            <trd>sadzi&cacute;</trd>
          </trdgrp>
        </subsnc>
        <subsnc mrk="plant.en0i.FIG">
          <uzo tip="stl">FIG</uzo>
          <dif>
            Penetrigi en la mensojn:
            <ekz>
              la sento pri honoro estas eduke en<tld/>ata &ccirc;e la
              Japanoj;
            </ekz><ekz>
              tiel la lernemo en<tld/>i&gcirc;as &ccirc;e tiuj infanoj;
            </ekz><ekz>
              akceptu kun humileco la en<tld/>itan vorton,
              kiu povas savi viajn animojn
              <fnt><bib>NT</bib>, <lok>&Jak; 1:21</lok></fnt>;
            </ekz><ekz>
              interne de la lando estas enplantata la ideo de Stalin pri
              <ctl>akri&gcirc;o de klasbatalo en proksimi&gcirc;o al la
                socialismo</ctl>
              <fnt>
                <bib>Metrop</bib>,
                <lok>Anta&ubreve;parolo (en la eldono de la &#xAB;Sezonoj&#xBB;).</lok>
              </fnt>.
            </ekz>
          </dif>
          <trd lng="hu">belepl&aacute;nt&aacute;l</trd>
          <trd lng="nl">inprenten</trd>
          <trd lng="pl">sia&cacute;</trd>
        </subsnc>
        <subsnc mrk="plant.en0i.KIR">
          <uzo tip="fak">KIR</uzo>
          <dif>
            Enmeti objekton (protezon, histon, grefton) en vivan
            korpon.
          </dif>
          <trdgrp lng="ru">
            <trd>&c_v;&c_zh;&c_i;&c_v;&c_i;&c_t;&c_mol;</trd>,
            <trd>&c_i;&c_m;&c_p;&c_l;&c_a;&c_n;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
          </trdgrp>
          <trdgrp lng="de">
            <trd>implantieren</trd>
          </trdgrp>
          <trdgrp lng="pl">
            <trd>szczepi&cacute;</trd>,
            <trd>wszczepia&cacute;</trd>,
            <trd>przeszczepia&cacute;</trd>
          </trdgrp>
        </subsnc>
        <trd lng="hu">j&oacute;l <ind>el&uuml;ltet</ind></trd>
        <adm>
          Hungaroj kontrolu, &ccirc;u la traduko plu ta&ubreve;gas post
          mia
          reverko.
          <aut>Sergio</aut>
        </adm>
      </snc>
      <trdgrp lng="be">
        <trd>&c_u;&c_zh;&c_y;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>,
        <trd>&c_u;&c_k;&c_a;&c_r;&c_a;&c_n;&c_ja;&c_c;&c_mol;</trd>,
        <trd>&c_ib;&c_m;&c_p;&c_l;&c_ja;&c_n;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
      </trdgrp>
  <trdgrp lng="cs">
    <trd>hluboce zako&rcaron;enit</trd>,
    <trd>implantovat</trd>,
    <trd>pevn&ecaron; zapustit</trd>,
    <trd>zasadit <klr>(rostlinu)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
        <trd>einpflanzen</trd>
      </trdgrp>
  <trd lng="en">implant</trd>
      <trd lng="fr">implanter</trd>
  <trdgrp lng="ja">
    <trd>&#x690D;&#x3048;&#x8FBC;&#x3080;  <pr>&#x3046;&#x3048;&#x3053;&#x3080;</pr></trd>,
    <trd>&#x690D;&#x3048;&#x3064;&#x3051;&#x308B;  <pr>&#x3046;&#x3048;&#x3064;&#x3051;&#x308B;</pr></trd>,
    <trd>&#x79FB;&#x690D;&#x3059;&#x308B;  <pr>&#x3044;&#x3057;&#x3087;&#x304F;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="ru">
        <trd>&c_v;&c_n;&c_je;&c_d;&c_r;&c_i;&c_t;&c_mol;</trd>,
        <trd>&c_p;&c_r;&c_i;&c_v;&c_i;&c_t;&c_mol;</trd>
      </trdgrp>
  <trd lng="sk">zasadi&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_a;&c_d;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_a;&c_d;&c_zh;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_a;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_shch;&c_je;&c_p;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_b;&c_i;&c_t;&c_i; &c_p;&c_je;&c_r;&c_je;&c_s;&c_a;&c_d;&c_k;&c_u;</trd>,
    <trd>&c_ib;&c_m;&c_p;&c_l;&c_a;&c_n;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

    <drv mrk="plant.trans0i">
      <kap>trans<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="plant.trans0i.KOMUNE">
        <dif>
          <tld lit="P"/>i ion en alian lokon:
        </dif>
        <subsnc mrk="plant.trans0i.HOR">
          <uzo tip="fak">HOR</uzo>
          <dif>
            Porti al nova, pli favora loko a&ubreve; grundo,
            kaj tie re<tld/>i:
            <ekz>
              <klr>(la pomarbo)</klr> fari&gcirc;is tiel granda, ke oni
              devis
              &gcirc;in trans<tld/>i en la &gcirc;ardenon
              <fnt><bib>Fab3</bib>, <lok>Nokta &ccirc;apo de
                  fra&ubreve;lo</lok></fnt>.
            </ekz>
            <refgrp tip="vid">
              <ref cel="prikl.0i.HOR">prikli</ref>
            </refgrp>
          </dif>
          <trd lng="fr">repiquer<klr> (des plantes)</klr></trd>
          <trdgrp lng="pl">
            <trd>przesadza&cacute;</trd>,
            <trd>pikowa&cacute;</trd>,
            <trd>flancowa&cacute;</trd>
          </trdgrp>
        </subsnc>
        <subsnc mrk="plant.trans0i.FIG">
          <uzo tip="stl">FIG</uzo>
          <dif>
            Penetrigi ideon a&ubreve; kutimon en alian spiriton
            a&ubreve;
            medion:
            <ekz>
              tiu supersti&ccirc;o trans<tld/>i&gcirc;is en la najbaran
              landon;
            </ekz><ekz>
              tiuj radikoj trans<tld/>i&gcirc;is el la franca en la
              anglan
              vortaron.
            </ekz>
          </dif>
          <trdgrp lng="de">
            <trd>umsetzen</trd>
            <trd>umtopfen</trd>
            <trd>&uuml;bertragen</trd>
          </trdgrp>
          <trd lng="pl">przeszczepia&cacute;</trd>
          <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_s;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>
        </subsnc>
        <subsnc mrk="plant.trans0i.KIR">
          <uzo tip="fak">KIR</uzo>
          <dif>
            Per operacio, anstata&ubreve;igi organon (a&ubreve; histon)
            de
            paciento per organo (a&ubreve; histo) de alia homo a&ubreve;
            animalo
            (a&ubreve; per histo el alia korpoparto).
          </dif>
          <trdgrp lng="de">
            <trd>transplantieren</trd>
          </trdgrp>
          <trdgrp lng="pl">
            <trd>transplantowa&cacute;</trd>,
            <trd>przeszczepia&cacute;</trd>
          </trdgrp>
          <trdgrp lng="ru">
            <trd>&c_p;&c_je;&c_r;&c_je;&c_s;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>,
            <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_l;&c_a;&c_n;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
          </trdgrp>
        </subsnc>
      </snc>
      <trdgrp lng="be">
        <trd>&c_p;&c_je;&c_r;&c_a;&c_s;&c_a;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_l;&c_ja;&c_n;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
      </trdgrp>
  <trdgrp lng="cs">
    <trd>p&rcaron;esadit</trd>,
    <trd>p&rcaron;esazovat</trd>
  </trdgrp>
  <trd lng="en">transplant</trd>
  <trd lng="es">trasplantar</trd>
  <trd lng="fr">transplanter</trd>
  <trd lng="hu">&aacute;t&uuml;ltet</trd>
  <trdgrp lng="ja">
    <trd>&#x79FB;&#x690D;&#x3059;&#x308B;  <pr>&#x3044;&#x3057;&#x3087;&#x304F;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x79FB;&#x4F4F;&#x3055;&#x305B;&#x308B;  <pr>&#x3044;&#x3058;&#x3085;&#x3046;&#x3055;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x79FB;&#x8EE2;&#x3055;&#x305B;&#x308B;  <pr>&#x3044;&#x3066;&#x3093;&#x3055;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
        <trd>transplanteren</trd>,
        <trd>verplanten</trd>,
        <trd>overbrengen</trd>
      </trdgrp>
  <trdgrp lng="sk">
    <trd>presadi&tcaron;</trd>,
    <trd>presadzova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_s;&c_a;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_l;&c_a;&c_n;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_s;&c_je;&c_l;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_n;&c_o;&c_s;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

    <drv mrk="plant.floro0o">
