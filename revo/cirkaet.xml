<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cirkaet.xml,v 1.7 2022/03/14 15:25:39 revo Exp $">
<kap>
  <rad>cirkaet</rad>/o
  <fnt>
    <bib>PIV3</bib>
  </fnt>
</kap>

<drv mrk="cirkaet.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="agl.serpent0o">Serpentaglo</ref>:
      <ekz>
        la <tld/>oj speciali&gcirc;is al serpentoj kaj lacertoj kiel predo.
      </ekz>
    </dif>
    <trd lng="ca"><ind>&agrave;guila</ind> marcenca</trd>
    <trd lng="en">short-toed <ind>eagle</ind></trd>
    <trd lng="pl">gado&zdot;er</trd>
    <trd lng="sv">orm<ind>&ouml;rn</ind></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>orel kr&aacute;tkoprst&yacute;</trd>,
    <trd>orl&iacute;k kr&aacute;tkoprst&yacute;</trd>
  </trdgrp>
  <trd lng="de">Schlangenadler</trd>
  <trdgrp lng="fr">
      <trd>circa&egrave;te</trd>,
      <trd>aigle des serpents</trd>
    </trdgrp>
  <trd lng="sk">hadiar kr&aacute;tkoprst&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x437;&#x43C;&#x456;&#x454;&#x457;&#x434;</trd>,
    <trd>&#x43A;&#x440;&#x430;&#x447;&#x443;&#x43D;</trd>
  </trdgrp>
</drv>

</art>

