<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kvodli.xml,v 1.18 2023/10/30 09:24:47 revo Exp $">
<kap>
   <rad>kvodlibet</rad>/o
   <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="kvodli.0o">
   <kap>
     <tld/>o
   </kap>
   <snc>
     <uzo tip="fak">MUZ</uzo>
     <dif>
      Muzika a&ubreve; literatura <ref tip="super" cel="verk.0o.propra">verko</ref>,
      konsistanta el pecoj de diversaj fontoj kaj karakteroj:
      <ekz>
        kiam ili kunvenis, oni tuj sonigis &hcirc;oralon, de tiu &ccirc;i pia komenco
        ili pasis al &scirc;ercoj ofte en forta kontrasto, t.e., ili tiam kantadis
        popularajn kantojn parte el komika kaj anka&ubreve; parte el maldeca enhavo,
        kunmiksante &ccirc;iujn spontanee &#8230; tian improvizan harmoniigon ili nomis <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=32931">Goldberg-variacioj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en 1965 <klr>[&#8230;]</klr> aperis la antologia verko Japana <tld lit="K"/>o, kun
        poeziaj, prozaj, teatraj kaj eseaj originala&jcirc;oj
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="pot.miks0ajxo">mikspoto</ref>
  </snc>
   <trd lng="be">&c_p;&c_a;&c_p;&c_u;&c_r;&c_y;</trd>
   <trd lng="ca">qu&ograve;dlibet</trd>
   <trdgrp lng="de">
     <trd>Quodlibet</trd>,
     <trd>Potpourri</trd>,
     <trd>Medley</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>quodlibet</trd>,
     <trd>medley</trd>,
     <trd>musical switch</trd>,
     <trd>potpourri</trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>egyveleg</trd>,
     <trd>quodlibet</trd>
   </trdgrp>
   <trdgrp lng="ja">
    <trd>&#12513;&#12489;&#12524;&#12540;</trd>,
    <trd>&#25509;&#32154;&#26354;  <pr>&#12379;&#12388;&#12382;&#12367;&#12365;&#12423;&#12367;</pr></trd>,
    <trd>&#23492;&#12379;&#38598;&#12417;  <pr>&#12424;&#12379;&#12354;&#12388;&#12417;</pr></trd>,
    <trd>&#38609;&#25991;&#38598;  <pr>&#12374;&#12388;&#12406;&#12435;&#12375;&#12421;&#12358;</pr></trd>,
    <trd>&#38609;&#37682;  <pr>&#12374;&#12388;&#12429;&#12367;</pr></trd>
  </trdgrp>
  <trd lng="nl">potpourri</trd>
   <trd lng="fr">pot-pourri</trd>
   <trdgrp lng="pl">
     <trd>quodlibet</trd>,
     <trd>kwodlibet</trd>
   </trdgrp>
   <trd lng="pt">quodlibet <klr>(m&uacute;s.)</klr></trd>
   <trd lng="ro">quodlibet</trd>
   <trd lng="ru">&c_p;&c_o;&c_p;&c_u;&c_r;&c_r;&c_i;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_p;&c_u;&c_r;&c_ib;</trd>,
    <trd>&c_v;&#8217;&c_ja;&c_z;&c_a;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
