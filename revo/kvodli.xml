<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kvodlibet</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kvodli.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzika a&ubreve; literatura verko, konsistanta el pecoj de diversaj fontoj
      kaj karakteroj.
    </dif>
  </snc>
  <trd lng="de">Quodlibet</trd>
  <trdgrp lng="en">
    <trd>quodlibet</trd>,
    <trd>medley</trd>,
    <trd>musical switch</trd>,
    <trd>potpourri</trd>
  </trdgrp>
  <trd lng="nl">potpourri</trd>
  <trd lng="ru">&c_p;&c_o;&c_p;&c_u;&c_r;&c_r;&c_i;</trd>
</drv>
</art>
