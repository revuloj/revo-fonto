<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <rad>kvodlibet</rad>/o
   <fnt>Z</fnt>
</kap>
<drv mrk="kvodli.0o">
   <kap>
     <tld/>o
   </kap>
   <snc>
     <uzo tip="fak">MUZ</uzo>
     <dif>
       Muzika a&ubreve; literatura verko, konsistanta el pecoj de 
diversaj fontoj
       kaj karakteroj.
     </dif>
   </snc>
   <trd lng="be">&c_p;&c_a;&c_p;&c_u;&c_r;&c_y;</trd>
   <trd lng="ca">qu&ograve;dlibet</trd>
   <trd lng="de">Quodlibet</trd>
   <trdgrp lng="en">
     <trd>quodlibet</trd>,
     <trd>medley</trd>,
     <trd>musical switch</trd>,
     <trd>potpourri</trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>egyveleg</trd>,
     <trd>quodlibet</trd>
   </trdgrp>
   <trd lng="nl">potpourri</trd>
   <trd lng="fr">pot-pourri</trd>
   <trd lng="pt">quodlibet <klr>(m&uacute;s.)</klr></trd>
   <trd lng="ru">&c_p;&c_o;&c_p;&c_u;&c_r;&c_r;&c_i;</trd>
</drv>
</art>
