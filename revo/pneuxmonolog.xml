<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pneuxmonolog.xml,v 1.9 2023/10/05 08:30:59 revo Exp $">
<kap>
    <rad>pne&ubreve;monolog</rad>/o <fnt><bib>PIV1</bib></fnt>,
    <var><kap><rad var="m">pne&ubreve;molog</rad>/o</kap></var>
</kap>
<drv mrk="pneuxmonolog.0o">
  <kap><tld/>o,
    <var><kap><tld var="m"/>o</kap></var>
  </kap>
  <snc mrk="pneuxmonolog.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="kurac.0isto">Kuracisto</ref>, kies fako
      estas <ref tip="vid" cel="pneuxmonologi.0o"><tld/>io</ref>:
      <ekz>
        <nom>Heinrich Herzog</nom> (1920&ndash;2014), sviso internisto,
        <tld var="m"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=563922"
              >Herzog</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        pulma embolio estas flegata de <tld var="m"/>o a&ubreve; specialisto pri
        internaj malsanoj en hospitalo per kontra&ubreve;-koaguli&gcirc;iloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=88163"
              >Pulma embolio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Pneumologe</trd>,
      <trd>Pulmologe</trd>,
      <trd>Lungenarzt</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_l;&c_mol;&c_m;&c_a;&c_n;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="en">pulmonologist</trd>
  <trd lng="es">neum&oacute;logo</trd>
  <trd lng="fr">pneumologue</trd>
  <trdgrp lng="nl">
    <trd>pneumoloog</trd>,
    <trd>longarts</trd>
  </trdgrp>
  <trd lng="pl">pneumolog</trd>
  <trd lng="pt">pneumologista</trd>
</drv>
</art>

