<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kvartir.xml,v 1.9 2023/08/08 15:27:39 revo Exp $">
<kap>
  <rad>kvartir</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="kvartir.0o">
  <kap><tld/>o</kap>
  <snc mrk="kvartir.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="dif" cel="kanton1.0o.MIL">Kantonmento</ref>:
      <ekz>
        kie ili mar&scirc;is, haltis en <tld/>o, /
        ofte la knabinoj ploris &ccirc;e l' foriro
        <fnt>
          <aut>S. Pet&odblac;fi, trad. K. Kalocsay</aut>,
          <vrk>Johano la brava</vrk>,
          <lok>1984</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni ricevis homindan man&gcirc;on kaj havis homindajn <tld/>ojn
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 39</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_mol;&c_m;&c_ja;&c_sh;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je; <klr>(&c_v;&c_a;&c_j;&c_s;&c_k;&c_o;&c_v;&c_a;&c_j; &c_ch;&c_a;&c_s;&c_mol;&c_c;&c_ib; &c_w; &c_n;&c_a;&c_s;&c_je;&c_l;&c_je;&c_n;&c_y;&c_m; &c_p;&c_u;&c_n;&c_k;&c_c;&c_je;)</klr></trd>
  <trd lng="ca">acantonament</trd>
  <trd lng="cs">ubytov&aacute;n&iacute; vojska</trd>
  <trdgrp lng="de">
    <trd>Truppenunterkunft</trd>,
    <trd>Unterbringung</trd>,
    <trd>Unterkunft <klr>(Milit&auml;r)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>billet</trd>,
    <trd>cantonment</trd>
  </trdgrp>
  <trd lng="fr">cantonnement<klr tip="ind"> (de troupe)</klr></trd>
  <trdgrp lng="hu">
    <trd>kv&aacute;rt&eacute;ly <klr>(katonai)</klr></trd>,
    <trd>sz&aacute;ll&aacute;sk&ouml;rlet</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>accantonamento<klr> (mil.)</klr></trd>,
    <trd>alloggiamento<klr> (mil.)</klr></trd>,
    <trd>quartiere<klr> (mil.)</klr></trd>
  </trdgrp>
  <trd lng="nl">kantonnement</trd>
  <trd lng="pl">kwatera</trd>
  <trd lng="ro">locuin&#539;&abreve;</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je; <klr>(&c_v;&c_o;&c_i;&c_n;&c_s;&c_k;&c_o;&c_j; &c_ch;&c_a;&c_s;&c_t;&c_i; &c_v; &c_n;&c_a;&c_s;&c_je;&c_l;&c_jo;&c_n;&c_n;&c_o;&c_m;
  &c_p;&c_u;&c_n;&c_k;&c_t;&c_je;)</klr></trd>
  <trd lng="sk">prechodn&yacute; vojensk&yacute; byt</trd>
</drv>

<drv mrk="kvartir.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kvartir.0i.MIL">
    <dif>
      <ref tip="dif" cel="kanton1.0i.MIL">Kantonmenti</ref>:
      <ekz>
        en la posto de la palaco <klr>[&#8230;]</klr> <tld/>is la Unua kohorto de la XII Fulma
        legio
        <fnt>
          <bib>MkM</bib>
          <lok>2</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam la malgrandnombra soldataro de <nom>Cortez</nom> envenis, Montezuma <tld/>igis ilin en
        la palacon de sia patro
        <fnt>
          <bib>SatirRak</bib>
          <lok>La barbaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu el <klr>[la]</klr> batalionoj ek<tld/>is en <nom>Kol&iacute;n</nom>
        <fnt>
          <aut>F. Ko&zcaron;&iacute;k, trad. J. Karen kaj J. Vondrou&scaron;ek</aut>,
          <vrk>La plej eminenta inter Pierotoj</vrk>,
          <lok>2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_mol;&c_m;&c_ja;&c_sh;&c_ch;&c_a;&c_c;&c_c;&c_a; <klr>(&c_a;&c_b; &c_v;&c_a;&c_j;&c_s;&c_k;&c_o;&c_v;&c_a;&c_j; &c_ch;&c_a;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  <trdgrp lng="de">
    <trd><ind>Quartier</ind> beziehen</trd>,
    <trd><ind>Unterkunft</ind> beziehen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kwaterowa&cacute;</trd>,
    <trd>stacjonowa&cacute;</trd>
  </trdgrp>
  <trd lng="ro">sta&#539;ionate</trd>
</drv>
</art>

