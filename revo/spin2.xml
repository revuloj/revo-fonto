<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: spin2.xml,v 1.20 2023/09/04 05:14:57 revo Exp $">
<kap>
  <rad>spin</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="spin2.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc mrk="spin2.0o.nombro">
    <dif>
      Nombro entjera a&ubreve; entjerplusduona, indikanta la
      transformadan karakteron de ondokampo:
      <ekz>
        mezono estas forte interaganta bosono (hadrono) kun entjera <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=251482">Mezono</url></lok>
        </fnt>.
      </ekz>
    </dif>
     <trd lng="en">spin</trd>
     <trd lng="es">esp&iacute;n</trd>
    <trd lng="hu">spin</trd>
    <trd lng="ru">&c_s;&c_p;&c_i;&c_n;</trd>
  </snc>
  <snc mrk="spin2.0o.momanto">
    <dif>
      <ref tip="dif" cel="spin2.0momanto"><tld lit="S"/>momanto</ref>:
      <ekz>
        antimaterio, konsistanta el antipartikloj kiuj havas kontra&ubreve;an &scirc;argon a&ubreve;
        <tld/>on ol la partikloj de la ordinara materio
        <fnt>
          <aut>OH</aut>
          <vrk><url ref="&Tekstaro;?nomo=artikoloj-el-monato">&Ccirc;u antimaterio
en la universo?</url></vrk>
<bib>Monato</bib>
<lok>2001/01, p. 23</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="spin.0o">spino (vertebraro)</ref>
  <trd lng="be">&c_s;&c_mol;&c_p;&c_ib;&c_n;</trd>
  <trd lng="ca">spin</trd>
  <trd lng="cs">p&aacute;te&rcaron;</trd>
  <trd lng="de">Spin</trd>
  <trd lng="en">spin</trd>
  <trd lng="hu">spin</trd>
  <trd lng="ja">&#x30B9;&#x30D4;&#x30F3;</trd>
  <trd lng="pl">spin</trd>
  <trd lng="ru">&c_s;&c_p;&c_i;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>chrbtica</trd>,
    <trd>chrbtov&aacute; kos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_h;&c_r;&c_je;&c_b;&c_je;&c_t;</trd>,
    <trd>&c_g;&c_r;&c_je;&c_b;&c_ib;&#x43D;&#x44C; (&c_g;&#x43E;&c_r;&c_i;)</trd>
  </trdgrp>
</drv>

<drv mrk="spin2.0momanto">
  <kap><tld/>momanto</kap>
  <snc>
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Momanto de movokvanto interna de elementa korpusklo.
    </dif>
  </snc>
  <snc>
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Magneta momanto de elementa korpusklo, proporcia al la <tld/>a
      momanto de movokvanto.
    </dif>
  </snc>
  <ref tip="vid" cel="momant.0o">momanto</ref>
  <trd lng="be">&c_s;&c_mol;&c_p;&c_ib;&c_n;</trd>
  <trd lng="ca">spin</trd>
  <trd lng="en">spin</trd>
  <trd lng="hu">spin</trd>
  <trd lng="ja">&#x30B9;&#x30D4;&#x30F3;&#x30E2;&#x30FC;&#x30E1;&#x30F3;&#x30C8;</trd>
  <trd lng="pl">spin</trd>
  <trd lng="ru">&c_s;&c_p;&c_i;&c_n;</trd>
  <trd lng="sk">spinov&yacute; moment</trd>
  <trd lng="cs">spinov&yacute; moment <klr>(hybnosti element&aacute;rn&iacute;ch &ccaron;&aacute;stic)</klr></trd>
</drv>
</art>
