<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>spin</rad>/o
</kap>
<drv mrk="spin2.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Nombro entjera a&ubreve; entjerplusduona, indikanta la transformadan
      karakteron de ondokampo.
    </dif>
    <trd lng="en">spin</trd>
    <trd lng="ru">&c_s;&c_p;&c_i;&c_n;</trd>
  </snc>
  <snc>
    <dif>
      <ref tip="dif" cel="spin2.0momanto"><tld lit="S"/>momanto</ref>.
    </dif>
  </snc>
  <trd lng="de">Spin</trd>
  <trd lng="en">spin</trd>
  <trd lng="ru">&c_s;&c_p;&c_i;&c_n;</trd>
</drv>

<drv mrk="spin2.0momanto">
  <kap><tld/>momanto</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Momanto de movokvanto interna de elementa korpusklo.
    </dif>
  </snc>
  <snc>
    <dif>
      Magneta momanto de elementa korpusklo, proporcia al la <tld/>a
      momanto de movokvanto.
    </dif>
  </snc>
  <ref tip="vid" cel="momant.0o">momanto</ref>
  <trd lng="en">spin</trd>
  <trd lng="ru">&c_s;&c_p;&c_i;&c_n;</trd>
</drv>
</art>
