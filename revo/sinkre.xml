<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>sinkretism</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sinkre.0o">
  <kap><tld/>o</kap>
  <snc mrk="sinkre.0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <uzo tip="fak">REL</uzo>
    <dif>
      Kunigo de diversdevenaj ideoj a&ubreve; kredoj en unu <ref tip="super"
      cel="doktri.0o.FIL">doktrinon</ref>.
      <!-- malnova difino:
      Konfuza kaj kompleksa kunigo de diversoriginaj ideoj, tezoj a&ubreve;
      kredoj en &scirc;ajne koheran tuton.
      -->
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_n;&c_k;&c_r;&c_e;&c_t;&c_y;&c_z;&c_m;</trd>
  <trd lng="ca">sincretisme</trd>
  <trdgrp lng="cs">
    <trd>filozofick&yacute;ch syst&eacute;m&uring;</trd>,
    <trd>n&aacute;bo&zcaron;enstv&iacute;</trd>,
    <trd>sm&ecaron;s n&aacute;zor&uring;</trd>,
    <trd>synkretizmus</trd>,
    <trd>synkreze</trd>
  </trdgrp>
  <trd lng="de">Synkretismus</trd>
  <trd lng="en">syncretism</trd>
  <trd lng="fa">&a_t;&a_l;&a_f;&f_y;&a_q;&zwnj;&f_g;&a_r;&a_A;&f_y;&f_y;</trd>
  <trd lng="fr">syncr&eacute;tisme</trd>
  <trd lng="hu">szinkretizmus</trd>
  <trd lng="nl">syncretisme</trd>
  <trd lng="ru">&c_s;&c_i;&c_n;&c_k;&c_r;&c_je;&c_t;&c_i;&c_z;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>synkretizmus</trd>,
    <trd>zmes n&aacute;zorov</trd>
  </trdgrp>
</drv>
</art>
