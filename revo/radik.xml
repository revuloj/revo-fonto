    <trd>&c_r;&c_a;&c_d;&c_i;&#x43A;&c_a;&#x43B;</trd>,
    <trd>&c_s;&c_i;&#x43C;&c_v;&#x43E;&#x43B; &#x43A;&#x43E;&c_r;&c_je;&#x43D;&#x44F;
    &#x43A;&c_v;&c_a;&c_d;&c_r;&c_a;&c_t;&#x43D;&#x43E;&c_g;&#x43E;</trd>
  </trdgrp>
</drv>

<drv mrk="radik.el0i">
  <kap>el<tld/>i <fnt>PV</fnt></kap>
  <snc>
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="radik.el0igi">El<tld/>igi</ref>:
      <ekz>
        <klr>[pro la]</klr> ventego <klr>[&#8230;]</klr> plej maljunegaj arboj el<tld/>i&gcirc;is
        kaj disbati&gcirc;is
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili el<tld/>is &ccirc;iujn arbetojn, kaj dis&jcirc;etis ilin renverse
        <fnt>
          <bib>Rob</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi vidis ilin riti kaj <klr>[&#8230;]</klr> &jcirc;eti en la akvon manplenojn el<tld/>itajn
        el la viskoza vegeta&jcirc;aro
        <fnt>
          <bib>Sol</bib>
        </fnt>;
      </ekz>
      <ekz>
        edukaj misioj <klr>[&#8230;]</klr> destinitaj al el<tld/>ado de la analfabetismo
        <fnt>
          <bib>MD05</bib>
          <vrk>La &leftquot;bolivara&rightquot; Venezuelo &dash; Partopreniga demokratio</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>odmocnit</trd>,
    <trd>odmoc&ncaron;ovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>entwurzeln</trd>,
    <trd>mit der Wurzel <ind>ausrei&szlig;en</ind></trd>
  </trdgrp>
  <trd lng="sk">odmocni&tcaron;</trd>
</drv>

<drv mrk="radik.el0igi">
  <kap>el<tld/>igi</kap>
  <snc mrk="radik.el0igi.eltiri">
    <dif>
      <ref tip="super" cel="tir.el0i">Eltiri</ref> el tero vegeta&jcirc;on kune kun &gcirc;iaj <tld/>oj:
      <ekz>
        kolektante la lolon, vi ne el<tld/>igu anka&ubreve; la tritikon
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 13:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni fal&ccirc;as kaj el<tld/>igas skirpojn kaj fragmitojn
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="https://tekstaro.com/t?nomo=artikoloj-el-monato">Feriumado &dash; &Gcirc;ojoj kaj &gcirc;uoj
          de finna somero</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>entwurzeln</trd>,
      <trd>mit der Wurzel <ind>ausrei&szlig;en</ind></trd>
    </trdgrp>
    <trd lng="fr">extirper</trd>
    <trdgrp lng="hu">
      <trd>gy&ouml;kerest&uuml;l kih&uacute;z</trd>,
      <trd>gy&ouml;kerest&uuml;l kit&eacute;p</trd>
    </trdgrp>
  </snc>
  <snc mrk="radik.el0igi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tute <ref tip="sin" cel="detru.0i">detrui</ref>, plene <ref tip="sin"
      cel="ekster1.0i">ekstermi</ref>:
      <ekz>
        el<tld/>igi el la urbo <klr>[&#8230;]</klr> &ccirc;iujn krimulojn
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 101:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tiu]</klr> fajro <klr>[&#8230;]</klr> mian tutan akirita&jcirc;on &gcirc;i el<tld/>igus
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 31:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        la celo de ilia militiro estis, el<tld/>igi la idolkulton de la &Hcirc;amidoj
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="likvid.0i">likvidi</ref>,
      <ref cel="neni.0igi.detrui">neniigi<sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>ausrotten</trd>,
      <trd>eradizieren</trd>
    </trdgrp>
    <trd lng="fr">&eacute;radiquer</trd>
    <trd lng="hu">gy&ouml;kerest&uuml;l kiirt</trd>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_k;&c_a;&c_r;&c_a;&c_n;&c_ja;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>vyko&rcaron;enit</trd>,
    <trd>vytrhnout s ko&rcaron;enem</trd>,
    <trd>vyvr&aacute;tit</trd>
  </trdgrp>
  <trd lng="nl">uitroeien</trd>
  <trd lng="ru">&c_i;&c_s;&c_k;&c_o;&c_r;&c_je;&c_n;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">vykoreni&tcaron;</trd>
</drv>

<drv mrk="radik.en0i">
  <kap>en<tld/>i <fnt>PV</fnt></kap>
  <snc>
    <uzo tip="stl">EVI</uzo>
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="dif" cel="radik.en0igi">En<tld/>igi</ref>:
      <ekz>
        forte en<tld/>ita kutimo
        <fnt>
          <bib>RdE</bib>
        </fnt>;
      </ekz>
      <ekz>
        la malfido al Irano restas en<tld/>ita
        <fnt>
          <bib>MD14</bib>
          <vrk>Senprecedenca koalicio, malfortaj &ccirc;eneroj</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
<trd lng="de">wurzeln</trd>
</drv>

<drv mrk="radik.en0igi">
  <kap>en<tld/>igi</kap>
  <snc mrk="radik.en0igi.fiksi">
    <dif>
      Fiksi vegeta&jcirc;on en tero per &gcirc;iaj <tld/>oj por ke &gcirc;i plue kresku:
      <ekz>
        la anta&ubreve;aj okupantoj estis postlasintaj <klr>[&#8230;]</klr> hederon en<tld/>igitan en
        malnova arbostumpo
        <fnt>
          <aut>Anna L&ouml;wenstein</aut>
          <vrk>La &scirc;tona urbo</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="plant.0i">planti</ref>
    </refgrp>
  </snc>
  <snc mrk="radik.en0igi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Firme kaj da&ubreve;re <ref tip="sin" cel="fiks.0i.kutimon">fiksi</ref>:
      <ekz>
        fido en viaj koroj <klr>[&#8230;]</klr> en<tld/>igite kaj bazite en amo
        <fnt>
          <bib>NT</bib>
          <lok>Efesanoj 3:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        la hereda principo estas tre profunde en<tld/>igita en la popolo
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        la militista rigoro <klr>[&#8230;]</klr> en<tld/>igis al li la konvinkon, ke &ccirc;iu
        ordono <klr>[&#8230;]</klr> devas esti plenumita
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiu tiko estis profunde en<tld/>igita en lia cerbo, &ccirc;ar neniam li povis sin
        liberigi el tiu manio
        <fnt>
          <bib>ChL</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="ankr.0i">ankri</ref>,
       <ref cel="famili1.0a">familiar</ref>igi,
       <ref cel="kutim.0igi">kutimigi</ref>,
       <ref cel="tradic.0i">tradicii</ref>
    </refgrp>
    <trd lng="de">verankern</trd>
  </snc>
  <trd lng="be">&c_u;&c_k;&c_a;&c_r;&c_a;&c_n;&c_ja;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>upevnit</trd>,
    <trd>usadit</trd>,
    <trd>zako&rcaron;enit</trd>
  </trdgrp>
  <trd lng="de">verwurzeln</trd>
  <trd lng="fr">enraciner</trd>
  <trdgrp lng="hu">
    <trd>meggy&ouml;kereztet</trd>,
    <trd>el&uuml;ltet</trd>
  </trdgrp>
  <trd lng="nl">wortel doen schieten</trd>
  <trd lng="ru">&c_u;&c_k;&c_o;&c_r;&c_je;&c_n;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>zakoreni&tcaron;</trd>,
    <trd>zapusti&tcaron; korene</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_u;&#x43A;&#x43E;&c_r;&c_ib;&#x43D;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_u;&#x43A;&#x43E;&c_r;&c_ib;&#x43D;&#x44F;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="radik.en0igxi">
  <kap>en<tld/>i&gcirc;i</kap>
  <snc mrk="radik.en0igxi.fiksigxi">
    <dif>
      Fiksi&gcirc;i en la tero per <tld/>oj:
      <ekz>
        la bran&ccirc;o en<tld/>i&gcirc;is, kaj <klr>[&#8230;]</klr> &scirc;i havis la senton, kvaza&ubreve; tio estas
        &scirc;ia arbo
        <fnt>
          <bib>Fab3</bib>
        </fnt>;
      </ekz>
    </dif>    
  </snc>
  <snc mrk="radik.en0igxi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Firme kaj da&ubreve;re fiksi&gcirc;i:
      <ekz>
        profunde enradiki&gcirc;is la pieco kaj diotimo de la hebreoj
        <fnt>
          <bib>RBa</bib>
        </fnt>;
      </ekz>
      <ekz>
        seriozeco profunde en<tld/>i&gcirc;is en &scirc;ia koro kaj menso
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        amo al la peko jam tro profunde en<tld/>i&gcirc;is en ilia animo
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fortik.0a">fortika</ref>,
      <ref cel="pov">vivpova</ref>,
      <ref cel="dauxr.0a">da&ubreve;ra</ref>.
    </refgrp>
    <trd lng="fr">s'<ind>enraciner</ind></trd>
    <trd lng="nl">wortel schieten</trd>
  </snc>
  <trd lng="be">&c_u;&c_k;&c_a;&c_r;&c_a;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="de">
    <trd>anwachsen</trd>,
    <trd>einwachsen</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gy&ouml;keret ereszt</trd>,
    <trd>meghonosodik</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_k;&c_o;&c_r;&c_je;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&#x43A;&#x43E;&c_r;&c_ib;&#x43D;&#x44E;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&c_u;&#x43A;&#x43E;&c_r;&c_ib;&#x43D;&#x44F;&c_t;&c_i;&c_s;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="radik.sen0a">
  <kap>sen<tld/>a</kap>
  <uzo tip="stl">FIG</uzo>
  <snc>
    <dif>
      Ne fiksita per radikoj; <klr>(figure)</klr> ne<ref tip="ant"
      cel="firm.0a.fiksita">firma</ref>,
      ne<ref tip="ant" cel="fortik.0a">fortika</ref>:
      <ekz>
        la homo <klr>[&#8230;]</klr> sen<tld/>a super tero, radikas malgra&ubreve; &ccirc;io en konscio pri
        homaj eblecoj
        <fnt>
          <bib>LOdE</bib>
          <vrk><url ref="https://tekstaro.com/t?nomo=ondo-de-esperanto">&Ccirc;u heroo &ndash; a&ubreve;
          koto?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        sen<tld/>a kosmopolito
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Senradika_kosmopolitismo">Senradika
          kosmopolitismo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        se Esperanto apartenas al neniu lando <klr>[&#8230;]</klr>, eksterulo tuj konkludas,
        ke la lingvo estas socie kaj kulture sen<tld/>a kaj tial <klr>[&#8230;]</klr>
        nevivipova
        <fnt>
          <vrk><url ref="http://www.eventoj.hu/arkivo/eve-155.htm">Eventoj</url></vrk>
          <lok>n-ro 155 - 1/septembro 1998</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_z; &c_k;&c_a;&c_r;&c_a;&c_n;&c_jo;&c_w;</trd>
  <trdgrp lng="de">
    <trd>ohne <ind>Wurzel</ind>n</trd>,
    <trd>entwurzelt</trd>,
    <trd>nicht <ind>verwurzelt</ind></trd>
  </trdgrp>
  <trd lng="hu">gy&ouml;k&eacute;rtelen</trd>
  <trd lng="ru">&c_b;&c_je;&c_z; &c_k;&c_o;&c_r;&c_n;&c_je;&c_j;</trd>
  <trd lng="uk">&c_b;&c_je;&c_z; &#x43A;&#x43E;&c_r;&c_je;&#x43D;&c_ib;&c_v;/&#x43A;&#x43E;&c_r;&c_ib;&#x43D;&#x43D;&#x44F;</trd>
</drv>

<drv mrk="radik.noa0o">
  <kap>n-a <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(de <ref cel="reel.0o.MAT">reelo</ref>
      <frm am="x"><k>x</k></frm>)</klr>
      Tia reelo <frm am="r"><k>r</k></frm>,
      ke <frm am="r^n=x"><k>r</k><sup><k>n</k></sup> = <k>x</k></frm>:
      <ekz>
        kvadrata, kuba <tld/>o <klr>(t.e. <frm>2</frm>-a,
        <frm>3</frm>-a)</klr>;
      </ekz>
      <ekz>
        la <frm am="n"><k>n</k></frm>-an <tld/>on de 
        <frm am="x"><k>x</k></frm> oni
        signas per <frm am="root(n)(x)"><sup><k>n</k></sup>&#x221A;<k>x</k></frm>
        a&ubreve; per <frm am="x^(1//n)"><k>x</k><sup>1/<k>n</k></sup></frm>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_r;&c_a;&c_n;&c_mol; &c_s;&c_t;&c_u;&c_p;&c_je;&c_n;&c_ib; n</trd>
  <trdgrp lng="cs">
    <trd>n-t&aacute; odmocnina</trd>,
    <trd>odmocnina n-t&eacute;ho stupn&ecaron;</trd>
  </trdgrp>
  <trd lng="de">n-te <ind>Wurzel</ind></trd>
  <trd lng="en">n-th <ind>root</ind></trd>
  <trd lng="fr">racine n-i&egrave;me</trd>
  <trd lng="hu">n-edik <ind>gy&ouml;k</ind></trd>
  <trdgrp lng="pl">
    <trd>pierwiastek n-tego stopnia</trd>,
    <trd>n-ty pierwiastek</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_je;&c_n;&c_mol; &c_s;&c_t;&c_je;&c_p;&c_je;&c_n;&c_i; n</trd>
  <trd lng="sk">n-t&aacute; odmocnina</trd>
</drv>

<drv mrk="radik.0signo">
  <kap><tld/>signo</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      Signo <frm am="sqrt(&#8230;)">&#x221A;</frm>:
      <ekz>
        Kartezio estis la unua matematikisto, kiu moderne kunigis la <tld/>signon kaj la
        vinkulon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=689478">Vinkulo (superstreko)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="radik.0ilo"><tld/>ilo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_n;&c_a;&c_k; &c_k;&c_o;&c_r;&c_a;&c_n;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_d;&c_y;&c_k;&c_a;&c_l;</trd>
  </trdgrp>
  <trd lng="cs">odmocn&iacute;tko</trd>
  <trd lng="de">Wurzelzeichen</trd>
  <trdgrp lng="en">
    <trd>root sign</trd>,
    <trd>radical sign</trd>
  </trdgrp>
  <trd lng="fr">radical<klr tip="ind"> (signe math.)</klr></trd>
  <trd lng="hu">gy&ouml;kjel</trd>
  <trdgrp lng="pl">
    <trd>pierwiastek</trd>,
    <trd>znak pierwiastka</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_n;&c_a;&c_k; &c_k;&c_o;&c_r;&c_n;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_d;&c_i;&c_k;&c_a;&c_l;</trd>
  </trdgrp>
  <trd lng="sk">znamienko odmocnenia</trd>
  <trd lng="uk">&c_z;&#x43D;&c_a;&#x43A; &#x43A;&#x43E;&c_r;&c_je;&#x43D;&#x44F;</trd>
</drv>

<drv mrk="radik.naz0o">
  <kap>naz<tld/>o</kap>
  <snc>
    <dif>
      Supra parto de <ref tip="malprt" cel="naz.0o">nazo</ref>,
      situanta &ccirc;e frunto:
      <ekz>
        reglati&gcirc;is la mallar&gcirc;a vertikala sulketo, fendinta la naz<tld/>on tiam, en
        oktobro, kiam malaperis la majstro
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        giganta &scirc;talblua viza&gcirc;o kun sola okulo super la naz<tld/>o, kiu per vertikala
        pupilo plena de neimagebla maliceco gapis al Atrejo
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Igramul', la Multulo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_je;&c_r;&c_a;&c_n;&c_o;&c_s;&c_mol;&c_s;&c_je;</trd>,
    <trd>&c_h;&c_r;&c_a;&c_p; <klr tip="amb">(&c_p;&c_je;&c_r;&c_a;&c_n;&c_o;&c_s;&c_mol;&c_s;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="cs">ko&rcaron;en nosu</trd>
  <trd lng="de">Nasenwurzel</trd>
  <trd lng="en">bridge<klr tip="amb"> (of nose)</klr></trd>
  <trd lng="fr">racine du nez</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_n;&c_o;&c_s;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">kore&ncaron; nosa</trd>
</drv>

