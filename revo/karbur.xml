<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: karbur.xml,v 1.15 2023/12/03 08:00:23 revo Exp $">
<kap>
    <rad>karbur</rad>/a&jcirc;o <fnt><bib>PIV3</bib></fnt>
</kap>

<drv mrk="karbur.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc>
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="brul.0igajxo">Bruliga&jcirc;o</ref>:
      <ekz>
        la intenco estis, per plikostigo de <tld/>a&jcirc;o redukti individuan
        a&ubreve;tomobiltrafikon
        <fnt>
          <aut>Stefan Maul</aut>
            <vrk><url ref="&Monato;/2001/007332.php">Memdetrua homaro</url></vrk>
            <bib>Monato</bib>
            <lok>2001/02, p. 4</lok>
          </fnt>;
      </ekz>
      <ekz>
        la irlanda registaro permesas, ke usonaj militaj aviadiloj
        surteri&gcirc;u kaj
        reprovizu sin en <nom>Shannon</nom> per <tld/>a&jcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2003/008027.html">Virina
          protest-tendaro</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        benzinon kaj dizelan <tld/>a&jcirc;on rafinitan el nafto plej ta&ubreve;ge
        kaj simple povas
        anstata&ubreve;i tiel nomata &leftquot;bio<tld/>a&jcirc;o&#x201D;, kiun liveras
        diversaj
        plantoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2007/009177.php">Bone
          nutrataj a&ubreve;toj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="fuel.0o">fuelo</ref>
    <rim>
      &Scirc;ajnas malkonsilinde uzi &leftquot;<tld/>a&jcirc;o&rightquot; sinonime
      al &leftquot;bruliga&jcirc;o&rightquot; a&ubreve;
      &leftquot;fuelo&rightquot;, &ccirc;ar la&ubreve;semantike
      oni subkomprenas la rezulton de <tld/>ado, t.e. <tld/>ita&jcirc;o, do la
      miksa&jcirc;o el
      bruliga&jcirc;o kaj gaso.
      <aut>Wolfram Diestel</aut>
    </rim>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ib;&c_v;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Treibstoff</trd>,
    <trd>Benzin</trd>
  </trdgrp>
  <trd lng="en">fuel</trd>
  <trdgrp lng="pl">
    <trd>paliwo</trd>,
    <trd>p&eogonek;dny materia&lstroke;</trd>
  </trdgrp> 
  <trd lng="ro">carburant</trd>
  <trdgrp lng="tr">
    <trd>yak&inodot;t</trd>,
    <trd>benzin</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&c_a;&#x43B;&#x44C;&#x43D;&c_a; &c_s;&c_u;&#x43C;&c_ib;&c_sh;</trd>
  <trd lng="zh">&#x77F3;&#x6CB9;&#x919A; <pr>sh&#xED;y&#xF3;um&#xED;</pr></trd>
</drv>

<drv mrk="karbur.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="karbur.0ilo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
     <ref tip="super" cel="aparat.0o.TEK">Aparato</ref>,
     pretiganta la miksa&jcirc;on de benzino kaj aero en eksplodomotoroj. 
    </dif>
    <trd lng="de">Vergaser</trd>
    <trdgrp lng="zh">
    <trd>&#x5316;&#x6CB9;&#x5668; <pr>hu&#xE0;y&#xF3;uq&#xEC;</pr></trd>,
    <trd>&#x6C14;&#x5316;&#x5668; <pr>q&#xEC;hu&#xE0;q&#xEC;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_b;&c_ju;&c_r;&c_a;&c_t;&c_a;&c_r;</trd>
  <trd lng="cs">karbur&aacute;tor</trd>
  <trd lng="fr">carburateur</trd>
  <trdgrp lng="ja">
    <trd>&#x30AD;&#x30E3;&#x30D6;&#x30EC;&#x30BF;&#x30FC;</trd>,
    <trd>&#x6C17;&#x5316;&#x5668;  <pr>&#x304D;&#x304B;&#x304D;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ga&zacute;nik</trd>,
    <trd>karburator</trd>
  </trdgrp> 
  <trd lng="ro">carburator</trd>
  <trd lng="sk">karbur&aacute;tor</trd>
  <trd lng="tr">karb&uuml;rat&ouml;r</trd>
  <trd lng="uk">&#x43A;&c_a;&c_r;&c_b;&#x44E;&c_r;&c_a;&c_t;&#x43E;&c_r;</trd>
</drv>

<drv mrk="karbur.0a">
  <kap>
    <tld/>a motoro
  </kap>
  <snc>
    <uzo tip="fak">TEK</uzo>
    <uzo tip="fak">AUT</uzo>
    <dif>
      <ref tip="super" cel="motor.0o.TEK">Motoro</ref>, en kiun oni
      en&scirc;prucas
      eksplodeman miksa&jcirc;on el bruliga&jcirc;o, ordinare benzino, kaj gaso
      (aero):
      <ekz>
        ma&scirc;inoj kiuj transformas varmon al mekanika energio
        <klr>[&#x2026;]</klr> estas la
        vaporma&scirc;ino, la gasturbino kaj la eksplodmotoro (ekz. <tld/>a
        motoro a&ubreve;
        dizela motoro)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=30532">Termodinamiko</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_b;&c_ju;&c_r;&c_a;&c_t;&c_a;&c_r;&c_n;&c_y; &c_r;&c_u;&c_h;&c_a;&c_v;&c_ib;&c_k;</trd>
  <trd lng="de">Ottomotor</trd>
  <trd lng="pl">silnik spalinowy</trd>     
  <trd lng="ro">motor cu ardere intern&abreve;</trd>
  <trd lng="tr">benzin motoru</trd>
  <trdgrp lng="zh">
    <trd>&#x70B9;&#x71C3;&#x5F0F;&#x53D1;&#x52A8;&#x673A; <pr>di&#x1CE;nr&#xE1;nsh&#xEC;f&#x101;d&#xF2;ngj&#x12B;</pr></trd>,
    <trd>&#x6C7D;&#x6CB9;&#x673A; <pr>q&#xEC;y&#xF3;uj&#x12B;</pr></trd>,
    <trd>&#x6C7D;&#x6CB9;&#x53D1;&#x52A8;&#x673A; <pr>q&#xEC;y&#xF3;uf&#x101;d&#xF2;ngj&#x12B;</pr></trd>
  </trdgrp>
</drv>

</art>

