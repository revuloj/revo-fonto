<kap>
  <ofc>8</ofc>
  <rad>tonel</rad>/o
</kap>
<drv mrk="tonel.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">SHI</uzo>
    <dif>
      Internacia <ref tip="lst" lst="voko:mezurunuoj" cel="unu.mezur0o">mezurunuo</ref>
      de <ref cel="volume.0o.FIZ">volumeno</ref>
      por <ref tip="malprt" cel="sxip.0o">&scirc;ipoj</ref>, valoranta 2,83
m&cubic;
      (100 kubaj britaj <ref tip="vid" cel="fut.0o">futoj</ref>):
      <ekz>
        &leftquot;Jeanie Johnson&#x201D; <klr>[&#x2026;]</klr> &#x2010; tiun vel&scirc;ipon, kiu tenis 700
        <tld/>ojn, konstruis skoto en 1847 en Kebekio
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="&Monato;/2004/008277.html">Renaski&gcirc;o
          de vel&scirc;ipo Jeanie Johnson</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_g;&c_ib;&c_s;&c_t;&c_r;&c_a;&c_v;&c_a;&c_ja;
  &c_t;&c_o;&c_n;&c_a;</trd>
  <trd lng="ca"><ind>tona</ind> Moorson</trd>
  <trdgrp lng="cs">
    <trd>RT</trd>,
    <trd>lodn&iacute; tuna <klr>(2,83 m3)</klr></trd>,
    <trd>registrovan&aacute; tuna</trd>,
    <trd>rejst&rcaron;&iacute;kov&aacute; tuna</trd>
  </trdgrp>
  <trd lng="de">Registertonne</trd>
  <trd lng="en">ton</trd>
  <trd lng="es">tonelada de arqueo</trd>
  <trdgrp lng="fr">
    <trd>tonneau</trd>,
    <trd>tonne de jauge</trd>
  </trdgrp>
  <trd lng="hu">regisztertonna</trd>
  <trd lng="nl">ton<klr tip="ind"> (inhoudsmaat v.schepen)</klr></trd>
  <trd lng="pl">tona rejestrowa</trd>
  <trd lng="pt">tonelada de arquea&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_r;&c_je;&c_g;&c_i;&c_s;&c_t;&c_r;&c_o;&c_v;&c_a;&c_ja;
  &c_t;&c_o;&c_n;&c_n;&c_a;</trd>
  <trd lng="sk">tona <klr>(priestorov&aacute;)</klr></trd>
  <trd lng="uk">&#x440;&#x435;&#x454;&#x441;&#x442;&#x440;&#x43E;&#x432;&#x430; &#x442;&#x43E;&#x43D;&#x43D;&#x430;</trd>
</drv>
