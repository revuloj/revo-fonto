<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>noktu</rad>/o
</kap>
<drv mrk="noktu.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="noktu.0o.birdo">
    <dif>
      Specio el la familio <ref tip="super" cel="strig.0edoj">strigedoj</ref>
      (<trd lng="la">Athene noctua</trd>).
    </dif>
    <trd lng="cs">s&yacute;c obecny</trd>
    <trd lng="de">Steinkauz</trd>
    <trd lng="en">little owl</trd>
    <trd lng="hu">kuvik</trd>
    <trd lng="pl">p&oacute;jd&zdot;ka</trd>
    <trd lng="ru">&c_d;&c_o;&c_m;&c_o;&c_v;&c_y;&c_j; &c_s;&c_y;&c_ch;</trd>
  </snc>
  <snc mrk="noktu.0o.papilio">
    <dif>
      Genro el familio <ref tip="super" cel="noktu.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Noctua</trd>), nokta papilio.
    </dif>
    <trd lng="de">Hausmutter <klr>k. a.</klr></trd>
  </snc>
</drv>
<drv mrk="noktu.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="lepido1.0oj">lepidopteroj</ref>
      (<trd lng="la">Noctuidae</trd>), de noktaj papilioj kun strigsimila
      koloraran&gcirc;o sur la anta&ubreve;aj flugiloj.
    </dif>
  </snc>
  <trd lng="de">Eulenschmetterlinge</trd>
</drv>
</art>
