<drv mrk="sulk.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
       Havanta <tld/>ojn:
      <ekz>
        en la <klr>[&#x2026;]</klr> sendormaj noktoj grizi&gcirc;is la haroj en la barbo kaj &ccirc;e
        la tempioj, la ha&ubreve;to fari&gcirc;is <tld/>a kaj flava
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        kapo, kiu sidas &ccirc;e longa <tld/>a kolo, la kapo de testudino
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Pramaljuna Morlino</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>furchig</trd>,
    <trd>runzlig</trd>,
    <trd>verschrumpelt</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>furrowed</trd>,
    <trd>wrinkled</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zmarszczony</trd>,
    <trd>pofa&lstroke;dowany</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x76B1;&#x7F29; <pr>zh&#xF2;usu&#x14D;</pr></trd>,
    <trd>&#x76BA;&#x7E2E; <pr>zh&#xF2;usu&#x14D;</pr></trd>,
    <trd>&#x76B1; <pr>zh&#xF2;u</pr></trd>,
    <trd>&#x76BA; <pr>zh&#xF2;u</pr></trd>,
    <trd>&#x76B1;&#x5DF4;&#x5DF4; <pr>zh&#xF2;ub&#x101;b&#x101;</pr></trd>,
    <trd>&#x76BA;&#x5DF4;&#x5DF4; <pr>zh&#xF2;ub&#x101;b&#x101;</pr></trd>,
    <trd>&#x76B1;&#x8936; <pr>zh&#xF2;uzh&#x11B;</pr></trd>,
    <trd>&#x76BA;&#x8936; <pr>zh&#xF2;uzh&#x11B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sulk.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="sulk.0i.sulkigi">
    <dif>
      <ref tip="dif" cel="sulk.0igi.falti"><tld lit="S"/>igi</ref>:
      <ekz>
        li <tld/>is la frunton, klakis malpacience per la fingroj
        <fnt>
          <bib>Marta</bib>
        </fnt>
      </ekz>
      <ekz>
        iliaj viza&gcirc;oj estas <tld/>itaj
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        arboj <klr>[kun]</klr> <tld/>ita &scirc;elo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        post &ccirc;iu rikolto oni ne per plugilo simple tra<tld/>as la grundon, sed kvaza&ubreve;
        muelas &gcirc;in &gcirc;is polvo
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2010/009930.php">Ni fosu nian konton!</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_s;&c_b;&c_r;&c_malmol;&c_ch;&c_k;&c_v;&c_a;&c_m;</trd>,
