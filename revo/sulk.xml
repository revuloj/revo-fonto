<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sulk</rad>/o
</kap>
<drv mrk="sulk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Longforma rekta fosa&jcirc;o farita per plugilo en la tero:
      <ekz>
        la plugilo tran&ccirc;as <tld/>ojn<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Furche</trd>,
      <trd>Rinne</trd>
    </trdgrp>
    <trd lng="nl">voor<klr tip="ind"> (in grond)</klr></trd>
    <trd lng="ru">&c_b;&c_o;&c_r;&c_o;&c_z;&c_d;&c_a;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Longforma rekta kava&jcirc;o a&ubreve; kava&jcirc;eto:
      <ekz>
        la vento plugas sur la rivero grandajn <tld/>ojn<fnt>B</fnt>;
      </ekz>
      <ekz>
        la tempo &ccirc;izis <tld/>ojn sur lia frunto;
      </ekz>
      <ekz>
        tempia <tld/>aro.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fald.0o0ajxo">faldo</ref>,
      <ref cel="falt.0o">falto</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Falte</trd>,
      <trd>Runzel</trd>
    </trdgrp>
    <trd lng="nl">rimpel</trd>
    <trdgrp lng="ru">
      <trd>&c_m;&c_o;&c_r;&c_shch;&c_i;&c_n;&c_a;</trd>,
      <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="sulk.0igi">
  <kap><tld/>(ig)i</kap>
  <snc>
    <dif>
      Fari <tld/>ojn:
      <ekz>
        li <tld/>is la frunton<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>igi la brovojn;
      </ekz>
      <ekz>
        venteto <tld/>etigis la akvon
      </ekz>
    </dif>
    <ref tip="sin" cel="plug.0i">plugi</ref>
  </snc>
  <trdgrp lng="nl">
    <trd>rimpelen</trd>,
    <trd>fronsen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_o;&c_r;&c_o;&c_z;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_shch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="sulk.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Pleni&gcirc;i per <tld/>oj:
      <ekz>
        viaj vangoj iam <tld/>i&gcirc;os;
      </ekz>
      <ekz>
        liaj brovoj mal<tld/>i&gcirc;is<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_shch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="sulk.voj0o">
  <kap>voj<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="S"/>o farita en vojo per radoj de veturiloj; vojkavo:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la plejmulto treni&gcirc;as en la voj<tld/>oj de malnovaj kutimoj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la voj<tld/>o de la rutino.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">wagenspoor</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_je;&c_ja;</trd>
</drv>

<drv mrk="sulk.sxov0o">
  <kap>&scirc;ov<tld/>o</kap>
  <snc>
    <dif>
      Entran&ccirc;o farita la&ubreve;longe en ligno a&ubreve; metalo por en&scirc;ovi ion en &gcirc;i
    </dif>
    <refgrp tip="sin">
      <ref cel="fold.0o">foldo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="nl">
    <trd>gleuf</trd>,
    <trd>groef</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_z;</trd>
</drv>
</art>
