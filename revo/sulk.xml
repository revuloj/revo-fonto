<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sulk</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sulk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sulk.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Longforma rekta fosa&jcirc;o farita per plugilo en la tero:
      <ekz>
        plugis plugistoj, faris siajn <tld/>ojn longaj
        <fnt><bib>MT</bib>, <lok>&Psa; 129:3</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Furche</trd>,
      <trd>Rinne</trd>
    </trdgrp> 
    <trd lng="hu">bar&aacute;zda</trd>
    <trd lng="nl">voor<klr tip="ind"> (in grond)</klr></trd>
    <trd lng="ru">&c_b;&c_o;&c_r;&c_o;&c_z;&c_d;&c_a;</trd>
  </snc>
  <snc mrk="sulk.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Longforma rekta kava&jcirc;o a&ubreve; kava&jcirc;eto:
      <ekz>
        la vento plugas sur la rivero grandajn <tld/>ojn<fnt>B</fnt>;
      </ekz>
      <ekz>
        la tempo &ccirc;izis <tld/>ojn sur lia frunto;
      </ekz>
      <ekz>
        tempia <tld/>aro.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fald.0o0ajxo">faldo</ref>,
      <ref cel="falt.0o">falto</ref>,
      <ref cel="fend.0o">fendo</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Falte</trd>,
      <trd>Runzel</trd>
    </trdgrp> 
    <trdgrp lng="en">
      <trd>wrinkle</trd>,
      <trd>groove</trd>
    </trdgrp>
    <trd lng="hu">r&aacute;nc</trd>
    <trd lng="nl">rimpel</trd>
    <trdgrp lng="ru">
      <trd>&c_m;&c_o;&c_r;&c_shch;&c_i;&c_n;&c_a;</trd>,
      <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <trd lng="en">furrow</trd>
  <trd lng="es">surco</trd> 
  <trdgrp lng="fr">
    <trd>ride</trd>,
    <trd>sillon</trd>
  </trdgrp>
  <trd lng="it">solco</trd>
</drv>

<drv mrk="sulk.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="sulk.0i.sulkigi">
    <ref tip="dif" cel="sulk.0igi.falti"><tld/>igi</ref>
    <ekz>
      li <tld/>is la frunton
      <fnt><bib>Marta</bib></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="fr">
    <trd>froncer</trd>,
    <trd>plisser</trd>,
    <trd>rider</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bar&aacute;zd&aacute;l</trd>,
    <trd>r&aacute;ncol</trd>
  </trdgrp>
  <trd lng="it">solcare</trd>
  <trdgrp lng="nl">
    <trd>rimpelen</trd>,
    <trd>fronsen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_o;&c_r;&c_o;&c_z;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_shch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="sulk.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sulk.0igi.falti">
    <dif>
      Fari <tld/>ojn:
      <ekz>
        la kronprinco <tld/>igis la brovojn kaj komencis malpacience
        pa&scirc;i
        sur la estrado
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 10a</lok></fnt>;
      </ekz>
      <ekz>
        venteto <tld/>etigis la akvon.
      </ekz>
    </dif>
    <ref tip="vid" cel="plug.0i">plugi</ref>
  </snc>
  <trdgrp lng="en">
    <trd>furrow</trd>,
    <trd>wrinkle</trd>
  </trdgrp>
  <trd lng="es">surcar</trd>
  <trdgrp lng="fr">
    <trd>froncer</trd>,
    <trd>plisser</trd>,
    <trd>rider</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bar&aacute;zd&aacute;l</trd>,
    <trd>r&aacute;ncol</trd>
  </trdgrp>
  <trd lng="it">solcare</trd>
  <trdgrp lng="nl">
    <trd>rimpelen</trd>,
    <trd>fronsen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_o;&c_r;&c_o;&c_z;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_shch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="sulk.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Pleni&gcirc;i per <tld/>oj:
      <ekz>
        viaj vangoj iam <tld/>i&gcirc;os;
      </ekz>
      <ekz>
        liaj brovoj mal<tld/>i&gcirc;is<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;inta maljunula manplato
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>furrow</trd>,
    <trd>wrinkle</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire des <ind>plis</ind></trd>,
    <trd>se <ind>plisser</ind></trd>,
    <trd>se <ind>rider</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bar&aacute;zd&aacute;l&oacute;dik</trd>,
    <trd>r&aacute;ncol&oacute;dik</trd>
  </trdgrp>
  <trd lng="it">solcarsi</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_shch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="sulk.sxov0o">
  <kap>&scirc;ov<tld/>o</kap>
  <snc mrk="sulk.sxov0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Entran&ccirc;o farita la&ubreve;longe en ligno a&ubreve; metalo por en&scirc;ovi ion en
      &gcirc;in.
    </dif>
    <refgrp tip="sin">
      <ref cel="fold.0o">foldo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="en">
    <trd>groove</trd>,
    <trd>slot</trd>
  </trdgrp>
  <trd lng="es">ranura</trd>
  <trd lng="fr">rainure</trd>
  <trd lng="hu">ereszt&eacute;khorony</trd>
  <trd lng="it">scanalatura</trd>
  <trdgrp lng="nl">
    <trd>gleuf</trd>,
    <trd>groef</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_z;</trd>
</drv>

<drv mrk="sulk.voj0o">
  <kap>voj<tld/>o</kap>
  <snc mrk="sulk.voj0o.TRA">
    <ref tip="dif" cel="kav.rado0o">radokavo</ref>
    <ekz>
      la paseroj saltetadis en la voj<tld/>oj
      <fnt><bib>Fab3</bib>, <lok>historio de la jaro</lok></fnt>;
    </ekz>
    <ekz>
      <uzo tip="stl">FIG</uzo>
      la voj<tld/>o de la rutino.
    </ekz>
  </snc>
  <trd lng="fr">orni&egrave;re</trd>
  <trd lng="hu">ker&eacute;knyom</trd>
  <trd lng="it">solco<klr> (traccia di ruote)</klr></trd>
  <trd lng="nl">wagenspoor</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_je;&c_ja;</trd>
</drv>
</art>
