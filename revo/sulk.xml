<drv mrk="sulk.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sulk.0igi.falti">
    <dif>
      Fari <tld/>ojn:
      <ekz>
        viaj vortoj estas malrespektaj diris la knabino <tld/>igante la brovojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        revemaj okuloj kaj strange noblaj trajtoj, kiujn neniam, &scirc;ajnis, <tld/>igis la kolero,
        a&ubreve; la timo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        rigardante la majestajn aleojn, &scirc;i revis <klr>[&#x2026;]</klr> sidi unu
        momenton en &ccirc;i tiu maro de verda&jcirc;o, kies supra&jcirc;on <tld/>igas ombroj kaj
        lumoj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        venteto <tld/>etigis la akvon.
      </ekz>
    </dif>
    <ref tip="vid" cel="plug.0i">plugi</ref>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_s;&c_b;&c_r;&c_malmol;&c_ch;&c_k;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_s;&c_m;&c_r;&c_malmol;&c_shch;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>furchen</trd>,
    <trd>in <ind>Falten</ind> legen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>furrow</trd>,
    <trd>wrinkle</trd>
  </trdgrp>
  <trd lng="es">surcar</trd>
  <trdgrp lng="fr">
    <trd>froncer</trd>,
    <trd>plisser</trd>,
    <trd>rider</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bar&aacute;zd&aacute;l</trd>,
    <trd>r&aacute;ncol</trd>
  </trdgrp>
  <trd lng="it">solcare</trd>
  <trdgrp lng="nl">
    <trd>rimpelen</trd>,
    <trd>fronsen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bru&zacute;dzi&cacute;</trd>,
    <trd>chmurzy&cacute; czo&lstroke;o</trd>,
    <trd>fa&lstroke;dowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_o;&c_r;&c_o;&c_z;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_shch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&#x43E;&c_r;&#x43E;&c_z;&#x43D;&c_i;&c_t;&c_i;</trd>,
    <trd>&#x43C;&#x43E;&c_r;&c_shch;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="sulk.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Pleni&gcirc;i per <tld/>oj:
      <ekz>
        viaj vangoj iam <tld/>i&gcirc;os;
