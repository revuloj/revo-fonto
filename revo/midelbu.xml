</ekz>
    </dif>
  </snc>
  <trd lng="ca">Middelburg</trd>
  <trd lng="de">Middelburg</trd>
  <trd lng="fr">Middelbourg</trd>
  <trd lng="hu">Middelburg</trd>
  <trd lng="nl">Middelburg</trd>
  <trd lng="pt">Middelburg</trd>
  </drv>
</art>

