<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>artemizi</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="artemi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="astera.0oj">asteracoj</ref>
      (<trd lng="la">Artemisia</trd>), el kiu diversaj specioj estas
      uzataj kiel spicherboj.
    </dif>
    <refgrp tip="sub">
      <ref cel="abrota.0o">abrotano</ref>,
      <ref cel="absint.0o">absinto</ref>,
      <ref cel="drakun.0o">drakunkolo</ref>
    </refgrp>
  </snc>
  <trd lng="de">Beifu&szlig;</trd>
  <trd lng="fr">armoise</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_y;&c_n;&c_mol;</trd>
  <trd lng="sv"><klr>(sl&auml;ktet) </klr>mal&ouml;rt</trd>
</drv>
</art>
