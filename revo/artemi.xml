<vortaro>
<art mrk="$Id$">
<kap>
<rad>artemizi</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="artemi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="aster.0acoj">asteracoj</ref>
      (<trd lng="la">Artemisia</trd>), el kiu diversaj specioj estas
      uzataj kiel spicherboj.  
    </dif>
    <refgrp tip="sub">
      <ref cel="abrota.0o">abrotano</ref>,
      <ref cel="absint.0o">absinto</ref>,
      <ref cel="drakun.0o">drakunkolo</ref>
    </refgrp>
  </snc>
  <trd lng="de">Beifu&szlig;</trd>
  <trd lng="ru">&#1087;&#1086;&#1083;&#1099;&#1085;&#1100;</trd>
</drv>
</art>
