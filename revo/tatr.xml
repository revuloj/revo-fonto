<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: tatr.xml,v 1.4 2024/03/21 06:28:31 afido Exp $">
    <kap>
      <rad>Tatr</rad>/oj
    </kap>

<drv mrk="tatr.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Slovaka-pola parto de <ref tip="malprt" cel="karpat.0oj">Karpatoj</ref>:
      <ekz>
        en 1949 vivis en <tld/>oj nur 160 &ccirc;amoj
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="https://www.monato.be/2013/010796.php">&Ccirc;a(r)moj en la parko</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni traveturis preska&ubreve; tutan et&scirc;puran reton
        de la <tld/>a Fervojo, uzis
        dentradan fervojon, kiu kunligas <tld/>ojn
        kun la &ccirc;efa fervojlinio
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#8470;4</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Tatra</trd>
    <trd lng="en">Tatra Mountains</trd>
    <trd lng="fr">Tatras</trd>
    <trd lng="pl">Tatry</trd>
    <trd lng="sk">Tatry</trd>
  </snc>
  <trd lng="be">&c_T;&c_a;&c_t;&c_r;&c_y;</trd>
  <trd lng="ja">&#x30BF;&#x30C8;&#x30E9;&#x5C71;&#33032; 
  <pr>&#x30BF;&#x30C8;&#x30E9;&#12373;&#12435;&#x307F;&#12419;&#x304F;</pr></trd>
</drv>

  </art>

