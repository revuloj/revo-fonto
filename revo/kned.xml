<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kned.xml,v 1.22 2023/12/03 09:58:51 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kned</rad>/i
</kap>

<drv mrk="kned.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <uzo tip="fak">KUI</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Tremante kaj kirlante miksi per la manoj farunon kaj akvon por
      fari paston.
    </dif>
    <trdgrp lng="be">
      <trd>&c_m;&c_ja;&c_s;&c_ib;&c_c;&c_mol; <klr tip="amb">(&c_c;&c_je;&c_s;&c_t;&c_a;)</klr></trd>,
      <trd>&c_r;&c_a;&c_sh;&c_ch;&c_y;&c_n;&c_ja;&c_c;&c_mol;</trd>
    </trdgrp>                  
    <trd lng="de">kneten</trd>
    <trd lng="es">amasar</trd>
    <trdgrp lng="en">
      <trd>mix</trd>,
      <trd>knead</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_m;&c_je;&c_s;&c_i;&c_t;&c_mol; <klr>(&c_t;&c_je;&c_s;&c_t;&c_o;)</klr></trd>,
      <trd>&c_z;&c_a;&c_m;&c_je;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Premadi kaj prilabori per la manoj pastecan substancon por
      doni al &gcirc;i ian formon:
      <ekz>
        <tld/>i argilon, potojn;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i <klr>(masa&gcirc;i)</klr> la muskolojn;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la cerbo de la infanoj estas <ind>facile <tld/>ebla</ind>
        <klr>(edukebla)</klr>;
        <trd lng="es">maleable</trd>
        <trd lng="fr">mall&eacute;able</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li <tld/>is lian animon per instruo kaj humiliga&jcirc;oj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        princo bela, <tld/>ita el mildo kaj pla&ccirc;o.
      </ekz>
    </dif>
    <trd lng="be">&c_l;&c_ja;&c_p;&c_ib;&c_c;&c_mol;</trd>
    <trd lng="de">formen</trd>
    <trd lng="es">moldear</trd>
    <trdgrp lng="en">
      <trd>form</trd>,
      <trd>give form to</trd>
    </trdgrp>
    <trd lng="ru">&c_l;&c_je;&c_p;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="br">merat</trd>
  <trdgrp lng="cs">
    <trd>hn&iacute;st</trd>,
    <trd>hn&ecaron;ten&iacute;m tvo&rcaron;it</trd>,
    <trd>m&iacute;sit</trd>
  </trdgrp>
  <trd lng="de">kneten</trd>
  <trdgrp lng="fr">
    <trd>p&eacute;trir</trd>,
    <trd>brasser<klr tip="ind"> (malaxer)</klr></trd>,
    <trd>fa&ccedil;onner</trd>,
    <trd>former<klr tip="ind"> (donner une forme avec les
    mains)</klr></trd>,
    <trd>malaxer</trd>,
    <trd>masser<klr tip="ind"> (p&eacute;trir)</klr></trd>,
    <trd>modeler<klr tip="ind"> (fa&ccedil;onner avec les mains)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3053;&#x306D;&#x308B;</trd>,
    <trd>&#x7DF4;&#x308B;  <pr>&#x306D;&#x308B;</pr></trd>,
    <trd>&#x3082;&#x3080;</trd>,
    <trd>&#x935B;&#x3048;&#x308B;  <pr>&#x304D;&#x305F;&#x3048;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">kneden</trd>
  <trdgrp lng="pl">
    <trd>gnie&sacute;&cacute; ciasto</trd>,
    <trd>miesi&cacute;</trd>,
    <trd>ugniata&cacute;</trd>
  </trdgrp>   
  <trd lng="ro">fr&abreve;m&acirc;nta aluatul</trd>
  <trdgrp lng="sk">
    <trd>hnies&tcaron;</trd>,
    <trd>miesi&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x456;&#x441;&#x438;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x438;&#x43C;&#x438;&#x43D;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x63C9;&#x5236; <pr>r&#xF3;uzh&#xEC;</pr></trd>,
    <trd>&#x640B; <pr>chu&#x101;i</pr></trd>,
    <trd>&#x6413;&#x63C9; <pr>cu&#x14D;r&#xF3;u</pr></trd>,
    <trd>&#x548C;&#x9762; <pr>hu&#xF3;mi&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kned.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <uzo tip="fak">KUI</uzo>
    <dif>
      Ujo, en kiu la bakisto <tld/>as la panon.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_ja;&c_zh;&c_a;</trd>,
    <trd>&c_d;&c_z;&c_je;&c_zh;&c_k;&c_a; <klr>(&c_d;&c_l;&c_ja; &c_c;&c_je;&c_s;&c_t;&c_a;)</klr></trd>
  </trdgrp>
  <trdgrp lng="br">    
    <trd>laouer-doaz</trd>,
    <trd>nev-doaz</trd>,
    <trd>toazeg</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>d&iacute;&zcaron;</trd>,
    <trd>d&iacute;&zcaron;e</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Knettrog</trd>,
    <trd>Backtrog</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>kneading bowl</trd>,
    <trd>kneading trough</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>masera</trd>,
    <trd>amasadera</trd>
  </trdgrp>
  <trd lng="fr">p&eacute;trin<klr tip="ind"> (de boulanger)</klr></trd>
  <trd lng="ja">&#x3053;&#x306D;&#x6876;  <pr>&#x3053;&#x306D;&#x304A;&#x3051;</pr></trd>
  <trd lng="nl">baktrog</trd>
  <trd lng="pl">dzie&zdot;a</trd>     
  <trdgrp lng="ro">
    <trd>covat&abreve;</trd>,
    <trd>bucluc</trd>
  </trdgrp>                                                             
  <trd lng="ru">&c_k;&c_v;&c_a;&c_sh;&c_n;&c_ja;</trd>
  <trd lng="sk">die&zcaron;a</trd>
  <trdgrp lng="uk">
    <trd>&#x434;&#x456;&#x436;&#x430;</trd>,
    <trd>&#x43D;&#x43E;&#x447;&#x432;&#x438;</trd>
  </trdgrp>
</drv>
</art>
