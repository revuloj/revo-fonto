<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kned</rad>/i
</kap>

<drv mrk="kned.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <uzo tip="fak">KUI</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Tremante kaj kirlante miksi per la manoj farunon kaj akvon por
      fari paston.
    </dif>
    <trdgrp lng="be">
      <trd>&c_m;&c_ja;&c_s;&c_ib;&c_c;&c_mol; <klr tip="amb">(&c_c;&c_je;&c_s;&c_t;&c_a;)</klr></trd>,
      <trd>&c_r;&c_a;&c_sh;&c_ch;&c_y;&c_n;&c_ja;&c_c;&c_mol;</trd>
    </trdgrp>                  
    <trd lng="de">kneten</trd>
    <trd lng="es">amasar</trd>
    <trdgrp lng="en">
      <trd>mix</trd>,
      <trd>knead</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_m;&c_je;&c_s;&c_i;&c_t;&c_mol; <klr>(&c_t;&c_je;&c_s;&c_t;&c_o;)</klr></trd>,
      <trd>&c_z;&c_a;&c_m;&c_je;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Premadi kaj prilabori per la manoj pastecan substancon por
      doni al &gcirc;i ian formon:
      <ekz>
        <tld/>i argilon, potojn;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i <klr>(masa&gcirc;i)</klr> la muskolojn;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la cerbo de la infanoj estas <ind>facile <tld/>ebla</ind>
        <klr>(edukebla)</klr>;
        <trd lng="fr">mall&eacute;able</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li <tld/>is lian animon per instruo kaj humiliga&jcirc;oj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        princo bela, <tld/>ita el mildo kaj pla&ccirc;o.
      </ekz>
    </dif>
    <trd lng="be">&c_l;&c_ja;&c_p;&c_ib;&c_c;&c_mol;</trd>
    <trd lng="de">formen</trd>
    <trd lng="es">moldear</trd>
    <trdgrp lng="en">
      <trd>form</trd>,
      <trd>give form to</trd>
    </trdgrp>
    <trd lng="ru">&c_l;&c_je;&c_p;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="br">merat</trd>
  <trd lng="de">kneten</trd>
  <trdgrp lng="fr">
    <trd>p&eacute;trir</trd>,
    <trd>brasser<klr tip="ind"> (malaxer)</klr></trd>,
    <trd>fa&ccedil;onner</trd>,
    <trd>former<klr tip="ind"> (donner une forme avec les
    mains)</klr></trd>,
    <trd>malaxer</trd>,
    <trd>masser<klr tip="ind"> (p&eacute;trir)</klr></trd>,
    <trd>modeler<klr tip="ind"> (fa&ccedil;onner avec les mains)</klr></trd>
  </trdgrp>
  <trd lng="nl">kneden</trd>
</drv>

<drv mrk="kned.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <uzo tip="fak">KUI</uzo>
    <dif>
      Ujo, en kiu la bakisto <tld/>as la panon.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_ja;&c_zh;&c_a;</trd>,
    <trd>&c_d;&c_z;&c_je;&c_zh;&c_k;&c_a; <klr>(&c_d;&c_l;&c_ja; &c_c;&c_je;&c_s;&c_t;&c_a;)</klr></trd>
  </trdgrp>             
  <trdgrp lng="br">    
    <trd>laouer-doaz</trd>,
    <trd>nev-doaz</trd>,
    <trd>toazeg</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Knettrog</trd>,
    <trd>Backtrog</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>kneading bowl</trd>,
    <trd>kneading trough</trd>
  </trdgrp>
  <trd lng="fr">p&eacute;trin<klr tip="ind"> (de boulanger)</klr></trd>
  <trd lng="nl">baktrog</trd>
  <trd lng="ru">&c_k;&c_v;&c_a;&c_sh;&c_n;&c_ja;</trd>
</drv>
</art>
