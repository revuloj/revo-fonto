<vortaro>
<art mrk="$Id$">
<kap>
  <rad>iniciator</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="inicia2.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <ref tip="dif" cel="inicia.0into">iniciatinto:</ref>
    <ekz>
      la <tld/>oj de la Esperanta afero eble ne &gcirc;isvivos &gcirc;is 
      tiu tempo, kiam fari&gcirc;os videblaj la fruktoj de ilia agado
      <fnt><bib>EE</bib></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="de">
    <trd>Initiator</trd>,
    <trd>Ansto&szlig;geber</trd>,
    <trd>Urheber</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>initiateur<klr tip="ind"> (promoteur)</klr></trd>,
    <trd>pionnier<klr tip="ind"> (initiateur)</klr></trd>,
    <trd>auteur<klr tip="ind"> (initiateur)</klr></trd>,
    <trd>cr&eacute;ateur<klr tip="ind"> (initiateur)</klr></trd>,
    <trd>promoteur<klr tip="ind"> (initiateur)</klr></trd>
  </trdgrp>
  <trd lng="nl">iniatiefnemer</trd>
  <trd lng="ru">&c_i;&c_n;&c_i;&c_c;&c_i;&c_a;&c_t;&c_o;&c_r;</trd>
</drv>
</art>
