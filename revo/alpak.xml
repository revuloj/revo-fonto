<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: alpak.xml,v 1.25 2019/06/30 07:47:08 revo Exp $">
<kap>
<rad>alpak</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="alpak.0o">
  <kap><tld/>o</kap>
  <snc mrk="alpak.0o.besto">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Hejma varia&jcirc;o de la guanaka
      <ref tip="super" cel="lam1.0o">lamo</ref>:
      <ekz>
        li posedas bienon, kiun li transformis al bredejo de <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="https://www.monato.be/2012/010528.php"
              >Bredi alpakojn</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        inkaoj <klr>[&#8230;]</klr> scipovis la arton teksi kaj produktis vestojn el la
        delikata lano de vikuno kaj <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=43815"
              >Inkaa imperio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="../bld/alpak.jpg"><tld/>o</bld>
    <trd lng="es">alpaca</trd>
    <trd lng="pl">alpaka</trd>
  </snc>
  <snc mrk="alpak.0o.lano">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Lano de tiu besto.
    </dif>
  </snc>
  <snc mrk="alpak.0o.teksajho">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Teksa&jcirc;o el tiu lano.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_p;&c_a;&c_k;&c_a;</trd>
  <trd lng="bg">&c_a;&c_l;&c_p;&c_a;&c_k;&c_a;</trd>
  <trd lng="ca">alpaca</trd>
  <trd lng="cs">alpaka</trd>
  <trd lng="de">Alpaka</trd>
  <trd lng="en">alpaca</trd>
  <trd lng="fa">&a_A_madda;&a_l;&f_p;&a_A;&f_k;&a_A;</trd>
  <trdgrp lng="fr">
    <trd>alpaca</trd>,
    <trd>alpaga</trd>
  </trdgrp>
  <trd lng="he">&alef;&lamed;&pe;&qof;&he;</trd>
  <trd lng="hr">alpaka</trd>
  <trd lng="hu">alpaka</trd>
  <trd lng="mk">&c_a;&c_l;&c_p;&c_a;&c_k;&c_a;</trd>
  <trd lng="nl">alpaca</trd>
  <trd lng="pt">alpaca</trd>
  <trd lng="ru">&c_a;&c_l;&c_mol;&c_p;&c_a;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>alpaka</trd>,
    <trd>vlna alebo l&aacute;tka z alpaky</trd>
  </trdgrp>
  <trd lng="sr">&c_a;&c_l;&c_p;&c_a;&c_k;&c_a;</trd>
  <trd lng="uk">&c_a;&c_l;&c_mol;&c_p;&c_a;&c_k;&c_a;</trd>
</drv>
</art>
