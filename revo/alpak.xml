<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: alpak.xml,v 1.31 2023/10/28 08:33:05 revo Exp $">
<kap>
  <rad>alpak</rad>/o
</kap>
<drv mrk="alpak.0o">
  <kap><tld/>o</kap>
  <snc mrk="alpak.0o.besto">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Hejma varia&jcirc;o de la gvanaka
      <ref tip="super" cel="lam1.0o">lamo</ref>:
      <ekz>
        li posedas bienon, kiun li transformis al bredejo de <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2012/010528.php">Bredi alpakojn</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        inkaoj <klr>[&#x2026;]</klr> scipovis la arton teksi kaj produktis vestojn el la
        delikata lano de vikuno kaj <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=43815">Inkaa imperio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="../bld/alpak.jpg"><tld/>o</bld>
    <trd lng="es">alpaca</trd>
    <trd lng="pl">alpaka</trd>
    <trdgrp lng="zh">
    <trd>&#x8278;&#x576D;&#x99AC; <pr>c&#x1CE;on&#xED;m&#x1CE;</pr></trd>,
    <trd>&#x8278;&#x576D;&#x9A6C; <pr>c&#x1CE;on&#xED;m&#x1CE;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="alpak.0o.lano">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="lan.0o">Lano</ref> de tiu besto.
    </dif>
    <trdgrp lng="zh">
    <trd>&#x7F8A;&#x99DD; <pr>y&#xE1;ngtu&#xF3;</pr></trd>,
    <trd>&#x7F8A;&#x9A7C; <pr>y&#xE1;ngtu&#xF3;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="alpak.0o.teksajho">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="teks.0ajxo.TEKS">Teksa&jcirc;o</ref> el tiu lano.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_p;&c_a;&c_k;&c_a;</trd>
  <trd lng="bg">&c_a;&c_l;&c_p;&c_a;&c_k;&c_a;</trd>
  <trd lng="ca">alpaca</trd>
  <trd lng="cs">alpaka</trd>
  <trd lng="de">Alpaka</trd>
  <trd lng="en">alpaca</trd>
  <trd lng="fa">&a_A_madda;&a_l;&f_p;&a_A;&f_k;&a_A;</trd>
  <trdgrp lng="fr">
    <trd>alpaca</trd>,
    <trd>alpaga</trd>
  </trdgrp>
  <trd lng="he">&alef;&lamed;&pe;&qof;&he;</trd>
  <trd lng="hr">alpaka</trd>
  <trd lng="hu">alpaka</trd>
  <trdgrp lng="ja">
    <trd>&#x30A2;&#x30EB;&#x30D1;&#x30AB;</trd>,
    <trd>&#x30A2;&#x30EB;&#x30D1;&#x30AB;&#x7E54;  <pr>&#x30A2;&#x30EB;&#x30D1;&#x30AB;&#x304A;</pr></trd>
  </trdgrp>
  <trd lng="mk">&c_a;&c_l;&c_p;&c_a;&c_k;&c_a;</trd>
  <trd lng="nl">alpaca</trd>
  <trd lng="pt">alpaca</trd>
  <trd lng="ru">&c_a;&c_l;&c_mol;&c_p;&c_a;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>alpaka</trd>,
    <trd>vlna alebo l&aacute;tka z alpaky</trd>
  </trdgrp>
  <trd lng="sr">&c_a;&c_l;&c_p;&c_a;&c_k;&c_a;</trd>
  <trd lng="uk">&c_a;&c_l;&c_mol;&c_p;&c_a;&c_k;&c_a;</trd>
</drv>
</art>
