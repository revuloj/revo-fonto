<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>alpak</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="alpak.0o">
  <kap><tld/>o</kap>
  <snc mrk="alpak.0o.besto">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Varia&jcirc;o de la guanaka
      <ref tip="super" cel="lam1.0o">lamo</ref>.
    </dif>
  </snc>
  <snc mrk="alpak.0o.lano">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Lano de tiu besto.
    </dif>
  </snc>
  <snc mrk="alpak.0o.teksajho">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Teksa&jcirc;o el tiu lano.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>alpaca</trd>,
    <trd>alpaga</trd>
  </trdgrp>
  <trd lng="ru">&#1072;&#1083;&#1100;&#1087;&#1072;&#1082;&#1072;</trd>
</drv>
</art>
