    <trd>efficace</trd>,
    <trd>op&eacute;rant</trd>
  </trdgrp>
  <trd lng="he">&yod;&#x5B8;&ayin;&#x5B4;&yod;&lamed;</trd>
  <trd lng="hu">hat&aacute;sos</trd>
  <trdgrp lng="nl">
