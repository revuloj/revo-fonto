<drv mrk="efik.post0o">
  <kap>post<tld/>o</kap>
  <snc mrk="efik.post0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      <tld lit="E"/>o, ofte nedezirata, kiu manifesti&gcirc;as
      post la unua, celata efiko:
      <ekz>
        trinku tion, &gcirc;i forigos la post<tld/>on
        de la gaso
        <fnt><bib>&Scirc;talrato</bib><lok>&ccirc;ap. 30</lok></fnt>;
      </ekz>
      <ekz>
        ni lernas &ccirc;iutage pli bone kompreni &gcirc;iajn
        le&gcirc;ojn kaj ekkoni la pli proksimajn kaj pli forajn
        post<tld/>ojn de niaj intervenoj en la kutiman iradon de la
        naturo
        <fnt>
        <aut>F. Engels, trad. Cezar</aut>
        <vrk><url ref="http://www.marxists.org/esperanto/marx-engels/1870s/1876junio.htm">La
        kontribuo de la laboro &ccirc;e la homi&gcirc;o de la
        simio</url></vrk>
        <lok>[vidita en 2013]</lok>
        </fnt>;
      </ekz>
      <ekz>
        la informkampanjo donis post<tld/>on per pliaj artikolo
        kaj televida programo
        <fnt>
        <aut>H. Gorecka</aut>
        <vrk>LOdE</vrk><lok>
        <url ref="&LOdE;Novaj/Nov03-30.htm">2003:11</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="efik.krom0o.SCI">kromefiko</ref>,
      <ref cel="sekv.0o">sekvo</ref>,
      <ref cel="sekv.0ajxo.MED">sekva&jcirc;o</ref>,
      <ref cel="sekv.post0i">postsekvo</ref>.
    </refgrp>
    <rim>
      &Ccirc;i tiu vorto ne aperas en PIV-oj.
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_s;&c_mol;&c_l;&c_ja;&c_d;&c_z;&c_je;&c_ja;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_d;&c_a;&c_l;&c_je;&c_j;&c_sh;&c_a;&c_je; <ind>&c_w;&c_z;&c_mol;&c_d;&c_z;&c_je;&c_ja;&c_n;&c_mol;&c_n;&c_je;</ind></trd>
  </trdgrp>
  <trd lng="de">Nachwirkung</trd>
  <trd lng="es">efecto secundario</trd>
  <trd lng="fr">effet secondaire</trd>
  <trdgrp lng="id">
    <trd><ind>efek</ind> samping</trd>,
    <trd>akibat</trd>
  </trdgrp>
  <trd lng="pl">skutek uboczny</trd>
  <trdgrp lng="zh">
    <trd>&#21518;&#21170; <pr>h&ograve;uj&igrave;n</pr></trd>,
    <trd>&#30452;&#25509;&#32080;&#26524; <pr>zh&iacute;ji&emacron;ji&eacute;gu&#466;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="efik.sen0a">
  <kap>
    sen<tld/>a
  </kap>
  <snc>
    <dif>
      Tia, ke &gcirc;i ne <tld/>as:
      <ekz>
        ondego trabatis tabulon, la pumpiloj estis sen<tld/>aj
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>;
      </ekz>
      <ekz>
        konfeso estus sen<tld/>a malkura&gcirc;a&jcirc;o, kiu min malhonorus kaj neutile frapus en
        koro Beatricon
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;APItro Dektria</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi helpis lin la&ubreve; mia eblo, ne per rezonado, &ccirc;ar &gcirc;i estas ordinare sen<tld/>a &ndash;
        sed per fakto: mi liveris pruvon
        <fnt>
          <bib>IKr</bib>
          <lok>Rakonto Pri Spiritoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        longa kaj sen<tld/>a interkonsilo
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XLIX</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_e;&c_f;&c_e;&c_k;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_d;&c_z;&c_je;&c_j;&c_s;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">wirkungslose</trd>
  <trd lng="en">ineffective</trd>
  <trd lng="es">ineficaz</trd>
  <trdgrp lng="fr">
    <trd>inefficace</trd>,
    <trd>inop&eacute;rant</trd>,
    <trd>vain</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>tak</ind> efektif</trd>,
    <trd><ind>tidak</ind> efektif</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#21177;&#26524;&#12398;&#12394;&#12356;  <pr>&#12371;&#12358;&#12363;&#12398;&#12394;&#12356;</pr></trd>,
    <trd>&#21177;&#21147;&#12398;&#12394;&#12356;  <pr>&#12371;&#12358;&#12426;&#12423;&#12367;&#12398;&#12394;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_m;&c_a;&c_r;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_t;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="efik.placeb0o">
  <kap>placeb<tld/>o, <var><kap>placeba <tld/>o</kap></var></kap>
  <snc mrk="efik.placeb0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Psikologia bonfartiga efiko konstatebla
      en <ref tip="vid" cel="placeb.0o.MED">placeboj</ref>
      kaj en &ccirc;iuj kuraciloj kaj kuracadoj:
      <ekz>
        la medicino de la pasinteco <klr>[&#8230;]</klr>
        preska&ubreve; tute bazi&gcirc;is sur la placebo<tld/>o
        <fnt>
          <aut>Tatoeba</aut>,
          <vrk><url ref="http://tatoeba.org/epo/sentences/show/1278421">
          Frazo nº1278421</url></vrk>,
          <lok>[vidita en 2011]</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u la klinikaj homeopatiaj efikoj
        estas placebaj <tld/>oj
        <fnt>
          <aut>-</aut>,
          <vrk><url
          ref="http://lamasson.com.br/biblioteca/biblioteca/pesquisahomeopatica/artikolo00n1metanalizo.htm">
          La Uzado de Meta Analizo...</url></vrk>,
          <lok>[vidita en 2011]</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_f;&c_e;&c_k;&c_t; &c_p;&c_l;&c_ja;&c_c;&c_e;&c_b;&c_a;</trd>
  <trd lng="de">Placeboeffekt</trd>
  <trd lng="fr">effet placebo</trd>
<trd lng="id"><ind>efek</ind> plasebo</trd>
</drv>

