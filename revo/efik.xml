<drv mrk="efik.0a">
  <kap><tld/>a</kap>
  <snc mrk="efik.0a.KOMUNE">
    <dif>
      <tld lit="E"/>anta:
      <ekz>
        la sa&gcirc;uloj el la lando ne povis trovi pli <tld/>an rimedon por malhelpi
        kompletan ruini&gcirc;on ol doni al la popolo belan religian punbruligon
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni aldonu al &gcirc;i ankora&ubreve; unu ingrediencon, por ke &gcirc;i estu pli <tld/>a
        <fnt>
          <bib>Marta</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> donis al &scirc;i kuracilon tiom <tld/>an, ke &scirc;i mortis
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        li trovis tre <tld/>an metodon akceli la laboron
        <fnt>
          <bib>HsT</bib>
          <lok>Feli&ccirc;a homo</lok>
        </fnt>;
      </ekz>
      <ekz>
        simplaj sed <tld/>aj hidra&ubreve;likaj ma&scirc;inoj, per kiuj oni povis &ccirc;erpi akvon kaj
        ver&scirc;i &gcirc;in sur kampojn
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pastraj pre&gcirc;oj <klr>[&#8230;]</klr> ne &ccirc;iam estis <tld/>aj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        plej <tld/>aj banlokoj por la bona humoro
        <fnt>
          <bib>Fab3</bib>
          <lok>Bona humoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        impresa kaj <tld/>a stilo
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        hako post hako estas la plej <tld/>a atako
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        pli <tld/>e ol bato pelas malsato
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ag.0ema">agema</ref>,
      <ref cel="drast.0a">drasta</ref>,
      <ref cel="energi.0a">energia</ref>,
      <ref cel="fort.0a.mano">forta<sncref/></ref>
    </refgrp>
    <refgrp tip="ant">
      <ref cel="van.0a">vana</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_je;&c_j;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_e;&c_f;&c_e;&c_k;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
