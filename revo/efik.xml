<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: efik.xml,v 1.35 2021/05/13 09:59:19 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>efik</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="efik.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="efik.0i.KOMUNE">
    <dif>
      Havi efektivan rezulton:
      <ekz>
        la ripozo bone <tld/>is al li, sur lin;
      </ekz>
      <ekz>
        se la jodo malbone <tld/>as &ccirc;e vi, ni uzos alian
        medikamenton;
      </ekz>
      <ekz>
        niaj klopodoj fine <tld/>is;
      </ekz>
      <ekz>
        pli <tld/>as ripro&ccirc;o &ccirc;e sa&gcirc;ulo ol cent
        batoj &ccirc;e malsa&gcirc;ulo
        <fnt><bib>MT</bib>, <lok>&Sen; 17:10</lok></fnt>;
      </ekz>
      <ekz>
        petego de justulo multe <tld/>as per sia energio
        <fnt>
          <bib>NT</bib>
          <lok>Jakobo 5:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        testamento <klr>[&#8230;]</klr> neniam <tld/>as, dum vivas la testamentinto
        <fnt>
          <bib>NT</bib>
          <lok>Hebreoj 9:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i komencis &scirc;iri la abomenindan urtikon, kiu <tld/>is kiel fajro
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_p;&c_l;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_z;&c_mol;&c_d;&c_z;&c_je;&c_j;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_je;&c_c;&c_mol;
    <ind>&c_e;&c_f;&c_e;&c_k;&c_t;</ind></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>tenir <ind>efecte</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>b&yacute;t &uacute;&ccaron;inn&yacute;</trd>,
    <trd>m&iacute;t &uacute;&ccaron;inek</trd>,
    <trd>p&uring;sobit</trd>,
    <trd>&uacute;&ccaron;inkovat</trd>
  </trdgrp>
  <trd lng="de">wirken</trd>
  <trd lng="en">effect</trd>
  <trdgrp lng="es">
    <trd>producir <ind>efecto</ind></trd>,
    <trd>actuar <klr>(producir un efecto)</klr></trd>,
    <trd>ser <klr>eficaz</klr></trd>,
    <trd>ser <klr>efectivo, -a</klr></trd>,
    <trd>hacer <ind>efecto</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>agir<klr tip="ind"> (produire un effet)</klr></trd>,
    <trd>exercer une <ind>action</ind></trd>,
    <trd>op&eacute;rer<klr tip="ind"> (produire un effet)</klr></trd>,
    <trd>produire un <ind>effet</ind></trd>
  </trdgrp>
  <trd lng="hu">hat<klr tip="amb"> (vmire, vkire)</klr></trd>
  <trdgrp lng="nl">
    <trd>werken<klr tip="ind"> (effect hebben)</klr></trd>,
    <trd>effect hebben</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>skutkowa&cacute;</trd>,
    <trd>dawa&cacute; <ind>efekt</ind></trd>,
    <trd>dawa&cacute; <ind>wynik</ind></trd>,
    <trd>dawa&cacute; <ind>rezultat</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>ser <ind>eficaz</ind></trd>,
    <trd>produzir <ind>efeito</ind></trd>,
    <trd>agir</trd>,
    <trd>atuar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_z;&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_v;&c_l;&c_i;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_m;&c_je;&c_t;&c_mol;
    &c_e;&c_f;&c_f;&c_je;&c_k;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ma&tcaron; &uacute;&ccaron;inok</trd>,
    <trd>zap&ocirc;sobi&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>verka</trd>,
    <trd>ha <ind>verkan</ind></trd>
  </trdgrp>
</drv>

<drv mrk="efik.0a">
  <kap><tld/>a</kap>
  <snc mrk="efik.0a.KOMUNE">
    <dif>
      <tld lit="E"/>anta:
      <ekz>
        <tld/>a kuracilo, rimedo, helpo, klopodo, ekzemplo,
        admono;
      </ekz>
      <ekz>
        simplaj sed <tld/>aj ma&scirc;inoj
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap.
        Anta&ubreve;parolo</lok></fnt>;
      </ekz>
      <ekz>
        <ind>sen<tld/>a</ind> kuracilo, konsilo, kolero;
        <trd lng="es">ineficaz</trd>
        <trdgrp lng="fr">
          <trd>inefficace</trd>,
          <trd>inop&eacute;rant</trd>,
          <trd>vain</trd>
        </trdgrp>
      </ekz>
      <ekz>
        hako post hako estas la plej <tld/>a atako
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        pli <tld/>e ol bato pelas malsato
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ag.0ema">agema</ref>,
      <ref cel="drast.0a">drasta</ref>,
      <ref cel="energi.0a">energia</ref>,
      <ref cel="fort.0a.mano">forta<sncref/></ref>;
    </refgrp>
    <refgrp tip="ant">
      <ref cel="van.0a">vana</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_je;&c_j;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_e;&c_f;&c_e;&c_k;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>efica&ccedil;</trd>,
    <trd>efectiu</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>efektn&iacute;</trd>,
    <trd>p&uring;sobiv&yacute;</trd>,
    <trd>v&yacute;konn&yacute;</trd>,
    <trd>&uacute;&ccaron;inn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wirksam</trd>,
    <trd>effektiv</trd>
  </trdgrp>
  <trd lng="en">effective<klr tip="amb"> (producing results)</klr></trd>
  <trdgrp lng="es">
    <trd>eficaz</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>actif<klr tip="ind"> (efficace)</klr></trd>,
    <trd>efficace</trd>,
    <trd>op&eacute;rant</trd>
  </trdgrp>
  <trd lng="he">&#x5D9;&#x5B8;&#x5E2;&#x5B4;&#x5D9;&#x5DC;</trd>
  <trd lng="hu">hat&aacute;sos</trd>
  <trdgrp lng="nl">
    <trd>doeltreffend</trd>,
    <trd>effectief</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>skuteczny</trd>,
    <trd>efektywny</trd>,
    <trd>skuteczny</trd>,
    <trd>sprawny</trd>,
    <trd>daj&aogonek;cy <ind>efekt</ind></trd>,
    <trd>daj&aogonek;cy <ind>wynik</ind></trd>,
    <trd>daj&aogonek;cy <ind>rezultat</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_e;&c_f;&c_f;&c_je;&c_k;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">&uacute;&ccaron;inn&yacute;</trd>
  <trd lng="sv">effektiv</trd>
</drv>

<drv mrk="efik.0o">
  <kap><tld/>o</kap>
  <snc mrk="efik.0o.ago_rezulto">
    <dif>
      Ago de tio, kio <tld/>as, <tld/>a povo a&ubreve; la rezulto de tiu ago:
      <ekz>
        vere teruran <tld/>on faris sur mi la glaso da pun&ccirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu floko da ne&gcirc;o, kiu falis sur &scirc;in, havis la saman <tld/>on, kiel se oni &jcirc;etas
        sur nin tutan &scirc;ovelamason
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la potencaj rimedoj havis nenian <tld/>on
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        ofte de ka&ubreve;zo senenhava venas <tld/>o plej
        grava<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        saniga&jcirc;o malbongusta, sed <tld/>o plej &gcirc;usta
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        lernado havas maldol&ccirc;an radikon, sed bonan <tld/>on
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="efekt.0o">efekto</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_z;&c_mol;&c_d;&c_z;&c_je;&c_ja;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_e;&c_f;&c_e;&c_k;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>efecte</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>efekt</trd>,
    <trd>p&uring;soben&iacute;</trd>,
    <trd>&uacute;&ccaron;inek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Wirkung</trd>,
    <trd>Effekt</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>efecto</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>action<klr tip="ind"> (effet)</klr></trd>,
    <trd>effet<klr tip="ind"> (action)</klr></trd>
  </trdgrp>
  <trd lng="he">&#x5D4;&#x5E9;&#x5E4;&#x5E2;&#x5D4;</trd>
  <trdgrp lng="hu">
    <trd>hat&aacute;s</trd>,
    <trd>effektus</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>effect</trd>,
    <trd>werking</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>efekt</trd>,
    <trd>skutek</trd>,
    <trd>sprawno&sacute;&cacute;</trd>,
    <trd>efektywno&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>,
    <trd>&c_v;&c_o;&c_z;&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>,
    <trd>&c_e;&c_f;&c_f;&c_je;&c_k;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>&uacute;&ccaron;innos&tcaron;</trd>,
    <trd>&uacute;&ccaron;inok</trd>
  </trdgrp>
</drv>

<drv mrk="efik.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="efik.0eco.KOMUNE">
    <dif>
      Kvalito de io <tld/>a:
      <ekz>
        &ccirc;iu rimedo, uzata tro longe, perdas sian <tld/>econ
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> ne tro fidas je la <tld/>eco de la sistemo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007272.html">Reen al la edena
          paradizo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        sekureco kaj <tld/>eco estas bu&ccirc;itaj sur la altaro de profitoj
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007260.html">Politiko
          elreligita</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_je;&c_j;&c_s;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_e;&c_f;&c_e;&c_k;&c_t;&c_y;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>efic&agrave;cia</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>efektivnost</trd>,
    <trd>p&uring;sobnost</trd>,
    <trd>v&yacute;konnost</trd>,
    <trd>&uacute;&ccaron;innost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Wirksamkeit</trd>,
    <trd>Effektivit&auml;t</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>eficacia</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>efficacit&eacute;</trd>,
    <trd>force<klr tip="ind"> (efficacit&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="he">&#x5D9;&#x5B0;&#x5E2;&#x5B4;&#x5D9;&#x5DC;&#x5D5;&#x5BC;&#x5EA;</trd>
  <trdgrp lng="nl">
    <trd>werking</trd>,
    <trd>effect</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>efektywno&sacute;&cacute;</trd>,
    <trd>skuteczno&sacute;&cacute;</trd>,
    <trd>sprawno&sacute;&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_f;&c_f;&c_je;&c_k;&c_t;&c_i;&c_v;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>efekt&iacute;vnos&tcaron;</trd>,
    <trd>&uacute;&ccaron;innos&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">effektivitet</trd>
</drv>

<drv mrk="efik.krom0o">
  <kap>krom<tld/>o,
  <var><kap>flank<tld/>o</kap></var></kap>
  <snc mrk="efik.krom0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Ne celita a&ubreve; mal&ccirc;efa <tld/>o de agado,
      de rimedo, kuracilo, operacio:
      <ekz>
        neniu scias, kiuj estas la krom<tld/>oj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="https://www.monato.be/2003/007991.html">Espero</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        grandaj akvobara&jcirc;oj por funkciigi hidroelektrajn centralojn estas mal&scirc;atataj de
        mediprotektantoj pro siaj krom<tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="https://www.monato.be/2007/009344.php">&Ccirc;u biokarbura&jcirc;oj helpas a&ubreve;
          malhelpas?</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        se la nova kemia&jcirc;o <klr>[&#8230;]</klr> ne havas probablajn
        malbonajn flank<tld/>ojn, eble estos permesite
        &gcirc;in vendi
        <fnt><aut>W. F. Rolt</aut>, <vrk>El la Polvo de la Tero</vrk>, 
        <lok>1967</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="epifenomen.0o.FIL">epifenomeno</ref>,
      <ref cel="efik.post0o.SCI">post<tld/>o</ref>
    </refgrp>
  </snc>
  <trd lng="cs">vedlej&scaron;&iacute; &uacute;&ccaron;inek</trd>
  <trdgrp lng="de">
    <trd>Nebenwirkung</trd>,
    <trd>Seiteneffekt</trd>
  </trdgrp>
  <trd lng="es">efecto colateral</trd>
  <trdgrp lng="fr">
    <trd>effet de bord<klr> (effet secondaire)</klr></trd>,
    <trd>effet secondaire</trd>
  </trdgrp>
  <trd lng="sk">ved&lcaron;aj&scaron;&iacute; &uacute;&ccaron;inok</trd>
</drv>

<drv mrk="efik.post0o">
  <kap>post<tld/>o</kap>
  <snc mrk="efik.post0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      <tld lit="E"/>o, ofte nedezirata, kiu manifesti&gcirc;as
      post la unua, celata efiko:
      <ekz>
        trinku tion, &gcirc;i forigos la post<tld/>on
        de la gaso
        <fnt><bib>&Scirc;talrato</bib><lok>&ccirc;ap. 30</lok></fnt>;
      </ekz>
      <ekz>
        ni lernas &ccirc;iutage pli bone kompreni &gcirc;iajn
        le&gcirc;ojn kaj ekkoni la pli proksimajn kaj pli forajn
        post<tld/>ojn de niaj intervenoj en la kutiman iradon de la
        naturo
        <fnt>
        <aut>F. Engels, trad. Cezar</aut>
        <vrk><url ref="http://www.marxists.org/esperanto/marx-engels/1870s/1876junio.htm">La
        kontribuo de la laboro &ccirc;e la homi&gcirc;o de la
        simio</url></vrk>
        <lok>[vidita en 2013]</lok>
        </fnt>;
      </ekz>
      <ekz>
        la informkampanjo donis post<tld/>on per pliaj artikolo
        kaj televida programo
        <fnt>
        <aut>H. Gorecka</aut>
        <vrk>LOdE</vrk><lok>
        <url ref="http://esperanto-ondo.ru/Novaj/Nov03-30.htm">2003:11</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="efik.krom0o.SCI">kromefiko</ref>,
      <ref cel="sekv.0o">sekvo</ref>,
      <ref cel="sekv.0ajxo.MED">sekva&jcirc;o</ref>,
      <ref cel="sekv.post0i">postsekvo</ref>.
    </refgrp>
    <rim>
      &Ccirc;i tiu vorto ne aperas en PIV-oj.
    </rim>
  </snc>
  <trd lng="de">Nachwirkung</trd>
  <trd lng="es">efecto secundario</trd>
  <trd lng="fr">effet secondaire</trd>
  <trd lng="pl">skutek uboczny</trd>
</drv>

<drv mrk="efik.placeb0o">
  <kap>placeb<tld/>o, <var><kap>placeba <tld/>o</kap></var></kap>
  <snc mrk="efik.placeb0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Psikologia bonfartiga efiko konstatebla
      en <ref tip="vid" cel="placeb.0o.MED">placeboj</ref>
      kaj en &ccirc;iuj kuraciloj kaj kuracadoj:
      <ekz>
        la medicino de la pasinteco <klr>[&#8230;]</klr>
        preska&ubreve; tute bazi&gcirc;is sur la placebo<tld/>o
        <fnt>
          <aut>Tatoeba</aut>,
          <vrk><url ref="http://tatoeba.org/epo/sentences/show/1278421">
          Frazo n&#xBA;1278421</url></vrk>,
          <lok>[vidita en 2011]</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u la klinikaj homeopatiaj efikoj
        estas placebaj <tld/>oj
        <fnt>
          <aut>-</aut>,
          <vrk><url
          ref="http://lamasson.com.br/biblioteca/biblioteca/pesquisahomeopatica/artikolo00n1metanalizo.htm">
          La Uzado de Meta Analizo...</url></vrk>,
          <lok>[vidita en 2011]</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
