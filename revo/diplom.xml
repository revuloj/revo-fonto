<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>diplom</rad>/o
</kap>
<drv mrk="diplom.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PED</uzo>
  <snc>
    <dif>
      Oficiala dokumento atestanta rajton, privilegion a&ubreve; rangan titolon:
      <ekz>
        pergamena, papirusa <tld/>o;
      </ekz>
      <ekz>
        <tld/>o pri nobeligo;
        <trd lng="hu">nemesi <ind>oklev&eacute;l</ind></trd>
      </ekz>
      <ekz>
        <tld/>o de profesoro;
      </ekz>
      <ekz>
        post sukcesa ekzameno li ricevis la <tld/>on de abituriento.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_p;&c_l;&c_jo;&c_m;</trd>
  <trdgrp lng="de">
    <trd>Diplom</trd>,
    <trd>Ernennungsurkunde</trd>,
    <trd>Verleihungsurkunde</trd>
  </trdgrp>
  <trd lng="es">diploma</trd>
  <trd lng="fr">dipl&ocirc;me</trd>
  <trdgrp lng="hu">
    <trd>oklev&eacute;l</trd>,
    <trd>okirat</trd>,
    <trd>diploma</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_i;&c_p;&c_l;&c_o;&c_m;</trd>,
    <trd>&c_a;&c_t;&c_t;&c_je;&c_s;&c_t;&c_a;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="diplom.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">PED</uzo>
  <snc>
    <dif>
      Alju&gcirc;i <tld/>on al:
      <ekz>
        <tld/>ita instruisto.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_p;&c_l;&c_ja;&c_m;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>diplomieren</trd>,
    <trd>ernennen</trd>,
    <trd>verleihen</trd>
  </trdgrp>
  <trd lng="fr">dipl&ocirc;mer</trd>
  <trdgrp lng="hu">
    <trd>oklev&eacute;llel t&uuml;ntet ki</trd>,
    <trd>oklev&eacute;llel l&aacute;t el</trd>,
    <trd>diplom&aacute;t ad</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_p;&c_u;&c_s;&c_k;&c_a;&c_t;&c_mol; &c_s; &c_d;&c_i;&c_p;&c_l;&c_o;&c_m;&c_o;&c_m;</trd>,
    <trd>&c_d;&c_i;&c_p;&c_l;&c_o;&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
