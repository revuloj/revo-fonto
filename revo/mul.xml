<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mul</rad>/o
</kap>
<drv mrk="mul.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Hibrido de azeno kaj &ccirc;evalino a&ubreve; de &ccirc;evalo kaj
azenino:
      <ekz>
        obstina kiel <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_l;</trd>
  <trdgrp lng="de">
    <trd>Maulesel <klr>(de azenino)</klr></trd>,
    <trd>Maultier <klr>(de &ccirc;evalino)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>baudet <klr>(de azenino)</klr></trd>,
    <trd>mulet <klr>(de &ccirc;evalino)</klr></trd>
  </trdgrp>
  <trd lng="hu">&ouml;szv&eacute;r</trd>
  <trdgrp lng="nl">
    <trd>muilezel <klr>(de azenino)</klr></trd>,
    <trd>muildier <klr>(de &ccirc;evalino)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>mulo <klr tip="amb">(zool.)</klr></trd>,
    <trd>burro <klr tip="amb">(zool.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_u;&c_l;</trd>
  <trd lng="es">mulo, mula</trd>
</drv>
</art>
