<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: mul.xml,v 1.19 2015/07/04 08:37:39 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>mul</rad>/o
    </kap>

    <drv mrk="mul.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="mul.0o.cxevalina">
        <dif>
          Ido de virazeno kaj &ccirc;evalino:
          <ekz>
            tiam levi&gcirc;is &ccirc;iuj filoj de la re&gcirc;o, kaj sidi&gcirc;is
            &ccirc;iu sur sia <tld/>o
            <fnt><bib>MT</bib>, <lok>&Sa2; 13:29</lok></fnt>.
          </ekz>
        </dif>
        <trd lng="de">Maultier</trd>
        <trd lng="fa">&a_q;&a_A;&a_T;&a_r;</trd>
        <trd lng="fr">mulet</trd>
        <trd lng="nl">muildier</trd>
        <trd lng="ru">&c_m;&c_u;&c_l;</trd>
      </snc>
      <snc mrk="mul.0o.hino">
        <ref tip="dif" cel="hin.0o">hino</ref>
        <trd lng="bg">&c_m;&c_u;&c_l;&c_je;</trd>
        <trd lng="de">Maulesel</trd>
        <trdgrp lng="fa">
          <trd>&a_q;&a_A;&a_T;&a_r; &f_p;&a_w;&a_z;&f_g;&f_y;</trd>,
          <trd>&a_kh;&a_r; &f_p;&a_w;&a_z;&f_g;&f_y;</trd>
        </trdgrp>
        <trd lng="fr">baudet</trd>
        <trd lng="nl">muilezel</trd>
        <trd lng="ru">&c_l;&c_o;&c_sh;&c_a;&c_k;</trd>
      </snc>
  <trd lng="be">&c_m;&c_u;&c_l;</trd>
  <trd lng="bg">&c_m;&c_u;&c_l;&c_je;</trd>
  <trd lng="cs">mula</trd>
  <trd lng="es">mulo, mula</trd>
  <trd lng="hu">&ouml;szv&eacute;r</trd>
  <trdgrp lng="pt">
        <trd>mulo <klr tip="amb">(zool.)</klr></trd>,
        <trd>burro <klr tip="amb">(zool.)</klr></trd>
      </trdgrp>
  <trd lng="sk">mul</trd>
</drv>
  </art>
