<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>globulari</rad>/o
</kap>
<drv mrk="globul.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="globul.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Globularia</trd>),
      kun bluaj floroj, havantaj laksigan efikon.
    </dif>
  </snc>
  <trd lng="de">Kugelblume</trd>
</drv>
<drv mrk="globul.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Globulariaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Kugelblumengew&auml;chse</trd>
</drv>
</art>
