<vortaro>
<art mrk="$Id$">
<kap>
  <rad>globulari</rad>/o
</kap>
<drv mrk="globul.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="globul1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Globularia</trd>),
      kun bluaj floroj, havantaj laksigan efikon.
    </dif>
  </snc>
  <trd lng="de">Kugelblume</trd>
  <trd lng="fr">globulaire<klr tip="ind"> (bot.)</klr></trd>
  <trd lng="nl">kogelbloem</trd>
  <trd lng="ru">&c_sh;&c_a;&c_r;&c_o;&c_v;&c_k;&c_a;</trd>
</drv>
</art>
