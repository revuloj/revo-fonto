<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: akant.xml,v 1.20 2023/08/30 18:23:31 revo Exp $">
<kap>
  <rad>akant</rad>/o
</kap>
<drv mrk="akant.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Vegeta&jcirc;o rimarkinda pro tre lar&gcirc;aj kaj elegante dentrandaj
      folioj, tipa stilelemento de kapiteloj ekde la 5a jc a.K.;
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">genro</ref> <trd lng="la">Acanthus</trd>:
      <ekz>
        blankaj trunkoj, jen florantaj sub la arkitravoj per akantaj floroj, jen
        volvitaj en jonstilajn kornojn
        <fnt>
          <bib>QuV</bib>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/f/fe/Acanthus_montanus_01.jpg/360px-Acanthus_montanus_01.jpg" prm="CC BY-SA 4.0">Monta <tld/>o, Botanika &Gcirc;ardeno, West Palm Beach, Florido, Usono
        <fnt>
          <aut>Katherine Wagner-Reiss, propra verko</aut>
          <lok><url ref="&WCW;/File:Acanthus_montanus_01.jpg">&WCW;/File:Acanthus_montanus_01.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_a;&c_n;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>akant</trd>,
    <trd>paznehtn&iacute;k</trd>
  </trdgrp>
  <trd lng="de">Akanthus</trd>
  <trd lng="en">acanthus</trd>
  <trd lng="fr">acanthe</trd>
  <trdgrp lng="he">
    <trd>&alef;&qof;&nun;&tet;&vav;</trd>,
    <trd>&samekh;&vav;&gimel; &ayin;&ftsadi;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>medvek&ouml;r&ouml;m</trd>,
    <trd>akantusz</trd>
  </trdgrp>
  <trd lng="ja">&#x30A2;&#x30AB;&#x30F3;&#x30B5;&#x30B9;</trd>
  <trd lng="nl">acanthus</trd>
  <trdgrp lng="pt">
    <trd>acanto</trd>,
    <trd>erva-gigante</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_k;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_a;&c_k;&c_a;&c_n;&c_t;&c_u;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>akant</trd>,
    <trd>&scaron;tylizovan&aacute; ozdoba na hlaviciach antick&yacute;ch st&lacute;pov</trd>
  </trdgrp>
  <trd lng="sv">akantus</trd>
  <trdgrp lng="uk">
    <trd>&#x430;&#x43A;&#x430;&#x43D;&#x442;</trd>,
    <trd>&#x430;&#x43A;&#x430;&#x43D;&#x444;</trd>
  </trdgrp>
</drv>
</art>
