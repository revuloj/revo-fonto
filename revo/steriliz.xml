<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: steriliz.xml,v 1.2 2025/01/17 06:33:46 afido Exp $">
    <kap>
      <rad>steriliz</rad>/i
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="steriliz.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">MED</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="dif" cel="steril.0igi">Steriligi</ref>:
      <ekz>
        <tld/>i kirurgiajn instrumentojn
        <fnt>
          <vrk><url ref="https://pola-retradio.org/2016/04/plasmo-en-la-purigado-de-spicoj/"
              >Plasmo en la purigado de spicoj</url></vrk>
          <lok>Pola Retradio, Apr. 12, 2016</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ne povas labori en la <tld/>ita atmosfero de laboratorio
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2003/008091.html"
              >Strebo al la efika figuro</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni uzas a&ubreve;toklavon kiel <tld/>ilo;
      </ekz>
      <ekz>
        en 1890 <nom>Koch</nom> faris filtra&jcirc;on de <tld/>ita kulturo de tuberkulozaj mikobakterioj,
        kiun li nomis tuberkulino
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2011/010389.php"
              >Dan&gcirc;era malamiko revenas</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">sterilisieren</trd>
  </snc>
</drv>

  </art>

