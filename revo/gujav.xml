<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>9</ofc><rad>gujav</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="gujav.0o">
  <kap><tld/>o</kap>
  <snc mrk="gujav.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="frukt.0o.KUI">Frukto</ref> de <tld/>arbo,
      proksimume globa, kun ordinare flaveta a&ubreve; verdeta 
      &scirc;elo, man&gcirc;ebla kaj preparebla kiel komfita&jcirc;o, 
      &jcirc;eleo, suko...:
      <ekz>
        estas glacia&jcirc;o el vinbero, ananaso, <tld/>o
        <fnt>
        <aut>L. Portella</aut>,
        <vrk>
        <url ref="http://www.lujz.org/amiko/parolado.php">
        &Scirc;i estas mia amiko</url></vrk>,
        <lok>Torixor&eacute;u Libroj, 2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Guave<klr tip="amb"> (Frucht)</klr></trd>
  <trd lng="en">guava</trd>
  <trd lng="fr">goyave</trd>
  <trdgrp lng="pl">
    <trd>gujawa<klr> (owoc)</klr></trd>,
    <trd>guawa<klr> (owoc)</klr></trd>,
    <trd>gwajawa<klr> (owoc)</klr></trd>,
    <trd>guajawa<klr> (owoc)</klr></trd>,
    <trd>gruszla<klr> (owoc)</klr></trd>
  </trdgrp>
  <trd lng="pt">goiaba</trd>
  <trd lng="tr">hintarmudu</trd>
</drv>

<drv mrk="gujav.0arbo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="gujav.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbo el arbaroj kaj riverbordoj de Centrameriko,
      kultivata en tropikaj landoj por la fruktoj,
      specio el familio mirtacoj
      (<trd lng="la">pisdium guayava</trd>):
      <ekz>
        li fiksis enviajn okulojn sur <tld/>o de tre proksima <tld/>arbo
        <fnt>
        <aut>G. Mattos</aut>,
        <vrk><url 
       ref="http://www.geocities.com/enchantedforest/2755/mat02l3.html">
        La petolema knabo</url></vrk>,
        <lok>1998</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Guave<klr tip="amb"> (Pflanze)</klr></trd>
  <trd lng="en">guava</trd>
  <trd lng="fr">goyavier<klr> (arbre)</klr></trd>
  <trdgrp lng="pl">
    <trd>gujawa<klr> (drzewo)</klr></trd>,
    <trd>guawa<klr> (drzewo)</klr></trd>,
    <trd>gwajawa<klr> (drzewo)</klr></trd>,
    <trd>guajawa<klr> (drzewo)</klr></trd>,
    <trd>gruszla<klr> (drzewo)</klr></trd>
  </trdgrp>
</drv>
</art>
