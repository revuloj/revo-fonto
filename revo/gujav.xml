<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gujav.xml,v 1.19 2022/04/30 08:42:41 revo Exp $">
<kap>
  <ofc>9</ofc><rad>gujav</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="gujav.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="gujav.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      La man&gcirc;ebla <ref tip="super" cel="frukt.0o.KUI">frukto</ref> de
      <tld/>arbo,
      proksimume globa, kun ordinare flaveta a&ubreve; verdeta 
      &scirc;elo:
      <ekz>
        <tld/>o estas preparebla kiel konfita&jcirc;o, &jcirc;eleo, suko;
      </ekz>
      <ekz>
        la postman&gcirc;oj konsistas el fruktoj ri&ccirc;aj je vitamino C, kiel
        papajo, <tld/>o kaj
        mandarino
        <fnt>
          <vrk><url ref="https://liberajbestoj.wordpress.com/page/2/">&leftquot;Nova&jcirc;oj pri veganismo en esperanto!&rightquot;</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        glacia&jcirc;o el vinbero, ananaso, <tld/>o
        <fnt>
          <aut>L. Portella</aut>,
          <vrk>
          <url ref="http://www.lujz.org/amiko/parolado.php">
          &Scirc;i estas mia amiko</url></vrk>,
          <lok>Torixor&eacute;u Libroj, 2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_a;&c_v;&c_a; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="bg">&c_g;&c_u;&c_a;&c_v;&c_a;</trd>
  <trd lng="ca">guaiaba</trd>
  <trd lng="cs">guave</trd>
  <trd lng="de">Guave<klr tip="amb"> (Frucht)</klr></trd>
  <trd lng="en">guava</trd>
  <trd lng="fr">goyave</trd>
  <trd lng="nl">guave</trd>
  <trdgrp lng="pl">
    <trd>gujawa<klr> (owoc)</klr></trd>,
    <trd>guawa<klr> (owoc)</klr></trd>,
    <trd>gwajawa<klr> (owoc)</klr></trd>,
    <trd>guajawa<klr> (owoc)</klr></trd>,
    <trd>gruszla<klr> (owoc)</klr></trd>
  </trdgrp>
  <trd lng="pt">goiaba</trd>
  <trd lng="sk">guave</trd>
  <trd lng="tr">hintarmudu</trd>
  <trd lng="uk">&#x491;&#x443;&#x430;&#x44F;&#x432;&#x430;&#xA0;(&#x43F;&#x43B;&#x456;&#x434; &#x456; &#x434;&#x435;&#x440;&#x435;&#x432;&#x43E;)</trd>
</drv>

<drv mrk="gujav.0arbo">
  <kap><ofc>9</ofc><tld/>arbo <fnt><bib>SPV</bib></fnt>,
  <var><kap><ofc>9</ofc><tld/>ujo
  <fnt><bib>PIV1</bib></fnt></kap></var></kap>
  <snc mrk="gujav.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbo el Centra Ameriko,
      kultivata en tropikaj landoj pro la fruktoj;
      <ref tip="lst" lst="voko:botanikaj_specioj" cel="speci.0o.BIO">specio</ref> el la 
      familio <ref tip="malprt" cel="mirtac.0oj">mirtacoj</ref> (<trd lng="la">Psidium guajava</trd>):
      <ekz>
        mi man&gcirc;os <tld/>on &ndash; respondas la frenezulo <klr>[&#x2026;]</klr>, sed
        tio ne estas <tld/>ujo, &gcirc;i estas fosto
        <fnt>
          <vrk><url ref="https://lernu.net/en/forumo/temo/19565">Man&gcirc;i gujavon / Humour / Forum - lernu.net</url></vrk>
        </fnt>!
      </ekz>
      <ekz>
        li fiksis enviajn okulojn sur <tld/>o de tre proksima <tld/>arbo
        <fnt>
          <aut>G. Mattos</aut>,
          <vrk><url ref="http://www.geocities.com/enchantedforest/2755/mat02l3.html">
          La petolema knabo</url></vrk>,
          <lok>1998</lok>
        </fnt>
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/6/67/Psidium_guajava_1.JPG/451px-Psidium_guajava_1.JPG" prm="CC BY-SA 3.0">folioj kaj floro de <tld/>ujo
        <fnt>
          <aut>Vinayaraj, propra verko</aut>
          <lok><url ref="&WCW;/File:Psidium_guajava_1.JPG">&WCW;/File:Psidium_guajava_1.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
    <trd lng="be">&c_g;&c_u;&c_a;&c_v;&c_a; <klr>(&c_d;&c_r;&c_e;&c_v;&c_a;)</klr></trd>
  <trd lng="bg">&c_g;&c_u;&c_a;&c_v;&c_a;</trd>
  <trd lng="ca">guaiaber</trd>
  <trdgrp lng="cs">
    <trd>guave <klr>(strom)</klr></trd>,
    <trd>kvaj&aacute;va</trd>
  </trdgrp>
  <trd lng="de">Guave<klr tip="amb"> (Pflanze)</klr></trd>
  <trd lng="en">guava</trd>
  <trd lng="fr">goyavier<klr> (arbre)</klr></trd>
  <trd lng="nl">guave</trd>
  <trdgrp lng="pl">
    <trd>gujawa<klr> (drzewo)</klr></trd>,
    <trd>guawa<klr> (drzewo)</klr></trd>,
    <trd>gwajawa<klr> (drzewo)</klr></trd>,
    <trd>guajawa<klr> (drzewo)</klr></trd>,
    <trd>gruszla<klr> (drzewo)</klr></trd>
  </trdgrp>
  <trd lng="pt">goiabeira</trd>
  <trd lng="sk">guave <klr>(strom)</klr></trd>
  <trd lng="uk">&#x491;&#x443;&#x430;&#x44F;&#x432;&#x430;&#xA0;(&#x434;&#x435;&#x440;&#x435;&#x432;&#x43E;)</trd>
</drv>
</art>
