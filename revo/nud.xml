<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nud.xml,v 1.36 2022/07/15 05:50:58 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>nud</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="nud.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="nud.0a.iu">
    <dif>
      Nevestita:
      <ekz>
        amba&ubreve; estis <tld/>aj <klr>[&#8230;]</klr> kaj ili ne hontis
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 2:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        li iris <tld/>a kaj <tld/>pieda
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 20:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        la filoj havis <tld/>an kolon kaj skotan kostumon
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili plejparte iris nudkape, nur malmultaj portis &ccirc;apon
        <fnt>
          <bib>VaK</bib>
          <lok>Dua &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu rabi eliras, ofte <tld/>a revenas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_g;&c_o;&c_l;</trd>
    <trdgrp lng="de">
       <trd>nackt</trd>,
       <trd>blo&szlig;</trd>
       <trd>entbl&ouml;&szlig;t</trd>
    </trdgrp>
    <trd lng="hu">meztelen</trd>    
    <trdgrp lng="nl">
      <trd>naakt</trd>,
      <trd>bloot</trd>
    </trdgrp>
    <trd lng="sv">naken</trd>
    <trd lng="zh">&#35064; [<ind>lu&#466;</ind>]</trd>
  </snc>
  <snc mrk="nud.0a.FIG_nekovrita">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Neprovizita de la ordinaraj kovra&jcirc;oj a&ubreve; garna&jcirc;oj:
      <ekz>
        <tld/>aj montetoj
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 41:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a <klr>(senfolia)</klr> arbo en la dezerto
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 17:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        la greno <klr>[&#8230;]</klr> estis fortran&ccirc;ita, nur la <tld/>aj sekaj stoploj elstaris
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        tie kreskis nenia floro, nenia herbeto, nur <tld/>a griza sabla tero sin etendis
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        radio de la subiranta suno enfalas en la 
        <tld/>an <klr>(senmeblan)</klr> kameron
        <fnt>
          <bib>Fab2</bib>
          <lok>Bildo el kastelremparo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <klr>[&#8230;]</klr> elser&ccirc;is la nestojn de la birdoj kaj rigardis la <tld/>ajn
        idojn
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        eniris en du vicoj deko da gvardiistoj 
        <klr>[&#8230;]</klr> kun <tld/>aj <klr>(elingigitaj)</klr> 
        glavoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        la teksto presita sur la etikedo estas nelegebla per 
        <tld/>a <klr>(neprovizita per okulvitro)</klr> okulo
        <fnt>
          <bib>KrB</bib>
          <lok>Ne tiel, sed tiel &ccirc;i!</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dezert.0a.FIG">dezerta</ref>,
      <ref cel="kalv.0a">kalva</ref>,
      <ref cel="plen.mal0a">malplena</ref>
    </refgrp>
    <trdgrp lng="de">
     <trd>ungesch&uuml;tzt</trd>,
     <trd>unbedeckt</trd>
    </trdgrp>
    <trdgrp lng="hu">
     <trd>csupasz</trd>,
     <trd>puszta <klr tip="ind">(&aacute;tv.)</klr></trd>
    </trdgrp>    
    <trd lng="sv">bar</trd>
    <trdgrp lng="nl">
      <trd>naakt</trd>,
      <trd>bloot</trd>
    </trdgrp>
  </snc>
  <snc mrk="nud.0a.FIG_senornama">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senornama:
      <ekz>
        ni tie &ccirc;i forlasos &ccirc;ian superfluan balaston 
        kaj parolos al vi nur en la nomo de
        la <tld/>a logiko
        <fnt>
          <bib>EE</bib>
          <lok>I</lok>
        </fnt>;
      </ekz>
      <ekz>
        multaj personoj donadis nur <tld/>ajn projektojn, li estis unua, kiu havis
        sufi&ccirc;e da pacienco, por ellabori plenan lingvon
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista 
          en Boulogne sur Mer en la 5a de a&ubreve;gusto 1905</vrk>
        </fnt>;
      </ekz>
      <ekz>
        diri al iu la <tld/>an
        <klr>(simplan, sinceran,
        <ref tip="vid" cel="rekt.0a">rekta</ref>n)</klr> veron
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="simpl.0a.senornama">simpla<sncref/></ref>,
      <ref cel="skelet.0a.FIG">skeleta<sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
       <trd>schlicht</trd>,
       <trd>einfach</trd>,
       <trd>rein</trd>,
       <trd>lauter</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>tiszta <klr tip="ind">(&aacute;tv.)</klr></trd>,
      <trd>puszta <klr tip="ind">(&aacute;tv.)</klr></trd>
    </trdgrp>   
    <trd lng="nl">onopgesmukt</trd>
    <trdgrp lng="sv">
      <trd>naken</trd>,
      <trd>bar</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_o;&c_l;&c_y;</trd>,
    <trd>&c_a;&c_g;&c_o;&c_l;&c_je;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bo">&t_da;&t_ma;&t_ra;&t_tsheg;&t_ha;&t_rata;&t_drengbu;&t_nga;&t_tsheg;&t_ba;&t_tsheg;</trd>
  <trd lng="br">noazh</trd>
  <trdgrp lng="ca">
    <trd>nu, nuet</trd>,
    <trd>descobert</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hol&yacute;</trd>,
    <trd>nah&yacute;</trd>
  </trdgrp>
  <trd lng="el">&gamma;&ypsilon;&my;&ny;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">naked</trd>
  <trd lng="es">desnudo</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_r;&a_h;&a_n;&a_h;</trd>,
    <trd>&a_l;&a_kh;&a_t;</trd>,
    <trd>&a_ayn;&a_r;&f_y;&a_A;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">nu</trd>
  <trdgrp lng="pl">
    <trd>nagi</trd>,
    <trd>go&lstroke;y</trd>,
    <trd>obna&zdot;ony</trd>
  </trdgrp> 
  <trdgrp lng="pt">
    <trd>nu</trd>,
    <trd>despido</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>gol</trd>,
    <trd>dezbr&abreve;cat</trd>,
    <trd>nud</trd>
</trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_a;&c_g;&c_o;&c_j;</trd>,
    <trd>&c_o;&c_b;&c_n;&c_a;&c_zh;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">obna&zcaron;en&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_o;&c_l;&c_je;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_l;&c_i;&c_s;&c_t;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="nud.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="nud.0igi.KOMUNE">
    <dif>
      <ref tip="dif" cel="vest.sen0igi">Senvestigi</ref>:
      <ekz>
        sur la malgrasa blanka brako, kiun <tld/>igis la mano de la patrino, vidi&gcirc;is
        nigre-blua makulo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> <tld/>igos la dentojn <klr>(rikanos)</klr> kaj apla&ubreve;dos
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igu viajn kapojn, stari&gcirc;u genue en la polvon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>!
      </ekz>
      <ekz>
        li <tld/>igas sin &gcirc;iskokse kaj dibo&ccirc;as en la fre&scirc;a akvo
        <fnt>
          <bib>LGA</bib>
          <lok>I.</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="nud.0igi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="sen.0igi">Senigi</ref> je kovra&jcirc;o, garna&jcirc;o, ornamo:
      <ekz>
        mi <tld/>igos <klr>(<ref cel="ing.el0igi">elingigos</ref>)</klr> post vi glavon
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 26:33</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu &ccirc;iam penis <tld/>igi <klr>(<ref cel="met.el0i.malsxirmi">elmeti</ref>, 
        <ref cel="kasx.mal0i">malka&scirc;i</ref>)</klr> la mizera&jcirc;on de la alia
        <fnt>
          <bib>Fab3</bib>
          <lok>En la lasta tago</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis konata pro sia emo publike <tld/>igi 
        <klr>(<ref cel="vual.sen0igi">senvualigi</ref>)</klr> malhonesta&jcirc;ojn
        <fnt>
          <bib>ChV</bib>
          <lok>10</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aper.0igi.vidigi">aperigi<sncref/></ref>,
      <ref cel="ekspon.0i.klarigi">eksponi<sncref/></ref>,
      <ref cel="montr.el0i.eksteren">elmontri<sncref/></ref>,
      <ref cel="kovr.mal0i.malkasxi">malkovri</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_g;&c_a;&c_l;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="br">noashaat</trd>
  <trdgrp lng="ca">
    <trd>despullar</trd>,
    <trd>descobrir</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>obna&zcaron;it</trd>,
    <trd>odhalit</trd>
  </trdgrp>
  <trd lng="de">entbl&ouml;&szlig;en</trd>
  <trd lng="es">desnudar</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_r;&a_h;&a_n;&a_h; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_l;&a_kh;&a_t; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;nuder</trd>
  <trdgrp lng="hu">
    <trd>lemeztelen&iacute;t</trd>,
    <trd>lecsupasz&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">ontbloten</trd>
  <trdgrp lng="pl">
    <trd>go&lstroke;oci&cacute;</trd>,
    <trd>ogo&lstroke;aca&cacute;</trd>
  </trdgrp>
  <trd lng="pt">despir</trd>
  <trdgrp lng="ro">
    <trd>goli&#539;i</trd>,
    <trd>scoate-o afar&abreve;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_n;&c_a;&c_zh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_g;&c_o;&c_l;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>obna&zcaron;i&tcaron;</trd>,
    <trd>odhali&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_o;&c_g;&c_o;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_g;&c_o;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_k;&c_r;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="nud.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="nud.0igxi.KOMUNE">
    <dif>
      I&gcirc;i <tld/>a, seni&gcirc;i je vestoj, je kovro:
      <ekz>
        &scirc;i kaj levis iom la randon de la tuko, <klr>[&#8230;]</klr> &scirc;ia mano <tld/>i&gcirc;is &gcirc;is la
        kubuto
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        li trinkis el la vino kaj ebrii&gcirc;is, kaj <tld/>i&gcirc;is en sia tendo
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 9:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi detruos la muron <klr>[&#8230;]</klr> tiel, ke <tld/>i&gcirc;os &gcirc;ia fundamento
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 13:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i&gcirc;is la fundamentoj de la universo, de la minaca vo&ccirc;o de la Eternulo
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 22:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        la betuloj flavi&gcirc;is kaj <tld/>i&gcirc;is
        <fnt>
          <bib>HsT</bib>
          <lok>Feli&ccirc;a homo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;iela volbo <tld/>i&gcirc;as
        <klr>(sennubi&gcirc;as)</klr>
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_g;&c_a;&c_l;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trd lng="de">sich <ind>entbl&ouml;&szlig;en</ind></trd>
  <trd lng="es"><ind>desnudar</ind>se</trd>
  <trd lng="fr">se <ind>d&eacute;nuder</ind></trd>
  <trd lng="pl">obna&zdot;y&cacute; si&eogonek;</trd>
  <trdgrp lng="ro">
    <trd>pr&abreve;dui</trd>,
    <trd>se dezv&abreve;luie</trd>
  </trdgrp>
  <trd lng="uk">&c_o;&c_g;&c_o;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
</drv>

<drv mrk="nud.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="nud.0ismo.MOR">
    <dif>
      Vivmaniero akceptanta <tld/>econ en ordinaraj cirkonstancoj,
      eventuale kontra&ubreve; sociaj kutimoj:
      <ekz>
        &ccirc;u vere, ke vi nenion scias pri <tld/>ismo
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>?
      </ekz>
      <ekz>
        ekkura&gcirc;inte mi demandis iun pri &ccirc;i tiu, 
        sed li tute ne sciis, kion signifas
        <tld/>ismo, kaj pluiris
        <fnt>
          <bib>VaK</bib>
          <lok>Sesa &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        en centra E&ubreve;ropo dum la vintro mi ne proponas la <tld/>ismon
        <fnt>
          <aut>P. Bowing</aut>,
          <vrk><url ref="http://www.ipernity.com/doc/elbertinum/3505816">-        
           </url></vrk>,
          <lok>2008-11-26</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_u;&c_d;&c_y;&c_z;&c_m;</trd>
  <trd lng="bg">&c_n;&c_u;&c_d;&c_i;&c_z;&c_malmol;&c_m;</trd>
  <trd lng="ca">nudisme</trd>
  <trd lng="cs">nudizmus</trd>
  <trd lng="de">Nudismus</trd>
  <trd lng="en">nudism</trd>
  <trd lng="es">nudismo</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_r;&a_h;&a_n;&a_h;&#8204;&f_g;&a_r;&a_A;&f_y;&f_y;</trd>,
    <trd>&a_ayn;&a_r;&f_y;&a_A;&a_n;&#8204;&f_g;&a_r;&a_A;&f_y;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">nudisme</trd>
  <trdgrp lng="nl">
    <trd>nudisme</trd>,
    <trd>naturisme</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>naturyzm</trd>,
    <trd>nudyzm</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>naturismului</trd>,
    <trd>nudism</trd>
  </trdgrp>
  <trd lng="sk">nudizmus</trd>
  <trd lng="uk">&c_n;&c_u;&c_d;&c_i;&c_z;&c_m;</trd>
</drv>

<drv mrk="nud.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="nud.0isto.MOR">
    <dif>
      <ref tip="vid" cel="nud.0ismo.MOR"><tld/>ism</ref>ano:
      <ekz>
        ni estas naturamikoj kaj <tld/>istoj
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        se eblas, ni ne uzas bankostumon, &ccirc;u neniam vi a&ubreve;dis pri 
        <tld/>istoj? ne gapu, ni estas <tld/>istoj
        <fnt>
          <aut>J. S&aacute;rk&ouml;zi</aut>,
          <vrk><url ref="http://mek.niif.hu/04100/04194/04194.htm">
          Filmo por pli ol 16 jaruloj kaj aliaj noveloj</url></vrk>, 
          <lok>1995</lok>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_u;&c_d;&c_y;&c_s;&c_t;</trd>
  <trd lng="bg">&c_n;&c_u;&c_d;&c_i;&c_s;&c_t;</trd>
  <trd lng="ca">nudista</trd>
  <trd lng="cs">nudista</trd>
  <trd lng="de">Nudist</trd>
  <trd lng="en">nudist</trd>
  <trd lng="es">nudista</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_r;&a_h;&a_n;&a_h;&#8204;&f_g;&a_r;&a_A;</trd>,
    <trd>&a_ayn;&a_r;&f_y;&a_A;&a_n;&#8204;&f_g;&a_r;&a_A;</trd>
  </trdgrp>
  <trd lng="fr">nudiste</trd>
  <trdgrp lng="nl">
    <trd>nudist</trd>,
    <trd>naturist</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>golec</trd>,
