<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>nud</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="nud.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="nud.0a.iu">
    <dif>
      Nevestita:
      <ekz>
        iri <tld/>piede, <tld/>kape.
      </ekz>
    </dif>
    <trd lng="de">nackt</trd>
    <trd lng="hu">meztelen</trd>    
    <trdgrp lng="nl">
      <trd>naakt</trd>,
      <trd>bloot</trd>
    </trdgrp>
    <trd lng="sv">naken</trd>
  </snc>
  <snc mrk="nud.0a.FIG_nekovrita">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Neprovizita de la ordinaraj kovra&jcirc;oj a&ubreve; garna&jcirc;oj:
      <ekz>
        <tld/>a (neprovizita per okulvitro) okulo;
      </ekz>
      <ekz>
        <tld/>a (elingigita) glavo;
      </ekz>
      <ekz>
        <tld/>a (senfolia) arbo;
      </ekz>
      <ekz>
        <tld/>a (nekovrita per kreska&jcirc;oj) tero;
      </ekz>
      <ekz>
        <tld/>a (senmebla) &ccirc;ambro;
      </ekz>
      <ekz>
        &gcirc;is<tld/>e tondita hundo.
      </ekz>
    </dif>
    <trdgrp lng="de">
     <trd>ungesch&uuml;tzt</trd>,
     <trd>unbedeckt</trd>
    </trdgrp>
    <trdgrp lng="hu">
     <trd>csupasz</trd>,
     <trd>puszta <klr tip="ind">(&aacute;tv.)</klr></trd>
    </trdgrp>    
    <trd lng="sv">bar</trd>
    <trdgrp lng="nl">
      <trd>naakt</trd>,
      <trd>bloot</trd>
    </trdgrp>
  </snc>
  <snc mrk="nud.0a.FIG_senornama">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senornama:
      <ekz>
        <tld/>a stilo, vero<fnt>B</fnt>,
      </ekz>
      <ekz>
        logiko<fnt>Z</fnt>;
      </ekz>
      <ekz>
        diri al iu la <tld/>an
        <klr>(simplan.
        <refgrp tip="vid">
          <ref cel="sincer.0a">sincera</ref>,
          <ref cel="rekt.0a">rekta</ref>
        </refgrp>) </klr>veron<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a <klr>(seka, skema, sen klarigoj)</klr>
        projekto<fnt>Z</fnt>
        (<refgrp tip="sin">
          <ref cel="skelet.0a">skeleta</ref>,
          <ref cel="sek.0a">seka</ref>)
        </refgrp>.
      </ekz>
    </dif>
    <trdgrp lng="de">
       <trd>rein</trd>,
       <trd>lauter</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>tiszta <klr tip="ind">(&aacute;tv.)</klr></trd>,
      <trd>puszta <klr tip="ind">(&aacute;tv.)</klr></trd>
    </trdgrp>   
    <trd lng="nl">onopgesmukt</trd>
    <trdgrp lng="sv">
      <trd>naken</trd>,
      <trd>bar</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_o;&c_l;&c_y;</trd>,
    <trd>&c_a;&c_g;&c_o;&c_l;&c_je;&c_n;&c_y;</trd>
  </trdgrp>                               
  <trd lng="br">noazh</trd>
  <trd lng="el">&gamma;&ypsilon;&my;&ny;&omikron_ton;&sigma_fina;</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_r;&a_h;&a_n;&a_h;</trd>,
    <trd>&a_l;&a_kh;&a_t;</trd>,
    <trd>&a_ayn;&a_r;&f_y;&a_A;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">nu</trd>
  <trdgrp lng="pt">
    <trd>nu</trd>,
    <trd>despido</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_a;&c_g;&c_o;&c_j;</trd>,
    <trd>&c_o;&c_b;&c_n;&c_a;&c_zh;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="nud.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="nud.0igi.KOMUNE">
    <dif>
      Igi <tld/>a:
      <ekz>
        <tld/>igi la brakon, ponardegon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la arbaro <tld/>i&gcirc;as;
      </ekz>
      <ekz>
        <tld/>i&gcirc;is la fundamento de la muro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>igi (senvualigi) la sekretan intrigon.
      </ekz>
    </dif>
  </snc>                      
  <trd lng="be">&c_a;&c_g;&c_a;&c_l;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="br">noashaat</trd>
  <trd lng="de">entbl&ouml;&szlig;en</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_r;&a_h;&a_n;&a_h; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_l;&a_kh;&a_t; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;nuder</trd>
  <trdgrp lng="hu">
    <trd>lemeztelen&iacute;t</trd>,
    <trd>lecsupasz&iacute;t</trd>
  </trdgrp>  
  <trd lng="nl">ontbloten</trd>
  <trd lng="pt">despir</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_n;&c_a;&c_zh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_g;&c_o;&c_l;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="nud.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="nud.0ismo.MOR">
    <dif>
      Vivmaniero akceptanta <tld/>econ en ordinaraj cirkonstancoj,
      eventuale kontra&ubreve; sociaj kutimoj:
      <ekz>
        en centra E&#468;ropo dum la vintro mi ne proponas la <tld/>ismon
        <fnt>
          <aut>P. Bowing</aut>,
          <vrk><url
          ref="http://www.ipernity.com/doc/elbertinum/3505816">-
          </url></vrk>,
          <lok>2008-11-26</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fa">
    <trd>&a_b;&a_r;&a_h;&a_n;&a_h;&zwnj;&f_g;&a_r;&a_A;&f_y;&f_y;</trd>,
    <trd>&a_ayn;&a_r;&f_y;&a_A;&a_n;&zwnj;&f_g;&a_r;&a_A;&f_y;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">nudisme</trd>
  <trdgrp lng="nl">
    <trd>nudisme</trd>,
    <trd>naturisme</trd>
  </trdgrp>
</drv>

<drv mrk="nud.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="nud.0isto.MOR">
    <dif>
      <ref tip="vid" cel="nud.0ismo.MOR"><tld/>ism</ref>ano:
      <ekz>
        se eblas, ni ne uzas bankostumon. &Ccirc;u neniam vi a&ubreve;dis pri 
        <tld/>istoj? Ne gapu, ni estas <tld/>istoj!
        <fnt>
          <aut>J. S&aacute;rk&ouml;zi</aut>,
          <vrk><url ref="http://mek.niif.hu/04100/04194/04194.htm">
          Filmo por pli ol 16 jaruloj kaj aliaj noveloj</url></vrk>, 
          <lok>1995</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fa">
    <trd>&a_b;&a_r;&a_h;&a_n;&a_h;&zwnj;&f_g;&a_r;&a_A;</trd>,
    <trd>&a_ayn;&a_r;&f_y;&a_A;&a_n;&zwnj;&f_g;&a_r;&a_A;</trd>
  </trdgrp>
  <trd lng="fr">nudiste</trd>
  <trdgrp lng="nl">
    <trd>nudist</trd>,
    <trd>naturist</trd>
  </trdgrp>
</drv>
</art>
