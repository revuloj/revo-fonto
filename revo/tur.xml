<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>tur</rad>/o
    </kap>
    <drv mrk="tur.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="tur.0o.ARKI">
        <uzo tip="fak">ARKI</uzo>
        <dif>
          Tre alta, ne tre vasta konstruo, plej ofte ronda
          a&ubreve; kvadrata:
          <ekz>
            <tld/>o de pre&gcirc;ejo;
          </ekz> <ekz>
            la Pariza Ejfel-<tld/>o
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="lum.0o">lumo</ref>.
        </refgrp>
        <trd lng="en">tower</trd>
        <trd lng="nl">toren</trd>
        <trd lng="ru">&c_b;&c_a;&c_sh;&c_n;&c_ja;</trd>
      </snc>
      <snc mrk="tur.0o.sxaka">
        <uzo tip="fak">SPO</uzo>
        <dif>
          &Scirc;akpeco kutime figurata per turo, movebla la&ubreve;
          vertikaloj a&ubreve; horizontaloj de la &scirc;aktabulo.
        </dif>
        <trd lng="ru">&c_l;&c_a;&c_d;&c_mol;&c_ja;</trd>
      </snc>
      <trd lng="de">Turm</trd>
    </drv>
  </art>
