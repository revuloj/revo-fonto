<subart>
  <dif>
    Memstara vortero kun la senco de
    <ref tip="vid" cel="en.0"><tld/></ref>:
  </dif>
<drv mrk="en.0a">
  <kap><tld/>a</kap>
  <snc mrk="en.0a.interna">
    <dif>
      <ref tip="dif" cel="intern.0a.malekstera">Interna</ref>:
      <ekz>
        sur la <tld/>a plasta ujo trovi&gcirc;is fingroprema&jcirc;oj nur de la junulo
        <fnt>
          <bib>ChC</bib>
        </fnt>;
      </ekz>      
      <ekz>
        &scirc;i bone konis la signojn de psika <tld/>a lukto, kaj tiujn &scirc;i legis el lia viza&gcirc;o
        <fnt>
          <bib>CKv</bib>
        </fnt>;
      </ekz>
      <ekz>
        li sciis en sia plej <tld/>a interno, ke <klr>[tio]</klr> neniam plu timigos lin
        <fnt>
          <bib>VRA</bib>
        </fnt>;
      </ekz>      
      <ekz>
        la <tld/>an militon <klr>[&#x2026;]</klr> en Kolombio oni povas spuri &gcirc;is la
        Sendependiga Milito
        <fnt>
          <bib>Kon11</bib>
        </fnt>;
      </ekz>
      <ekz>
        la konkaj jarringoj mem ne nur indikas la a&gcirc;on de la <tld/>a besto, sed ja enhavas
        multe da informoj pri klimato en la pasinteco
        <fnt>
          <bib>Monato</bib>
          <aut>Oliver Kim</aut>
          <vrk><url ref="&Monato;/2014/011359.php">Rekorda konkulo</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>      
    </dif>
    <trdgrp lng="de">
      <trd>innen</trd>,
      <trd>darinnen</trd>,
      <trd>drinnen</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">vnit&rcaron;n&iacute;</trd>
  <trdgrp lng="en">
      <trd>inside<klr tip="ind"> (inner)</klr></trd>,
      <trd>inner</trd>
    </trdgrp>
  <trdgrp lng="es">
      <trd>interior</trd>,
      <trd>interno</trd>
    </trdgrp>
  <trdgrp lng="fr">
      <trd>(de) <ind>dedans</ind><klr tip="ind"> (interne)</klr></trd>,
      <trd>int&eacute;rieur<klr tip="ind"> (interne)</klr></trd>,
      <trd>interne</trd>
    </trdgrp>
  <trd lng="he">&pe;&nun;&yod;&mem;&yod;</trd>
  <trdgrp lng="pl">
      <trd>w</trd>,
      <trd>wewn&aogonek;trz</trd>
    </trdgrp>
  <trd lng="ru">&c_v;&c_n;&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_i;&c_j;</trd>
  <trd lng="sk">vn&uacute;torn&yacute;</trd>
  <trd lng="tok">insa</trd>
  <trd lng="uk">&#x432;&#x43D;&#x443;&#x442;&#x440;&#x456;&#x448;&#x43D;&#x456;&#x439;</trd>
</drv>
  <drv mrk="en.0e">
  <kap><tld/>e</kap>
  <snc mrk="en.0e.interne">
    <dif>
      <ref tip="dif" cel="intern.0e">Interne</ref>:
      <ekz>
        la &scirc;rankopordoj skui&gcirc;is kvaza&ubreve; <tld/>e eksplodus grenado
        <fnt>
          <bib>ChB</bib>
        </fnt>;
      </ekz>
      <ekz>
        pasta&jcirc;o kun far&ccirc;o <tld/>e
        <fnt>
          <bib>Kon11</bib>
        </fnt>;
      </ekz>
      <ekz>
        miaj a&jcirc;oj estis tute malordigitaj <tld/>e kaj ekster la valizoj
        <fnt>
          <bib>Kon11</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bo">&t_na;&t_nga;&t_tsheg;&t_la;&t_tsheg;</trd>
  <trd lng="cs">uvnit&rcaron;</trd>
  <trdgrp lng="de">
    <trd>innen</trd>,
    <trd>innerhalb</trd>
  </trdgrp>
  <trd lng="en">inside<klr tip="ind"> (adv.)</klr></trd>
  <trd lng="es">hacia dentro<klr tip="ind"> (adv.)</klr></trd>
  <trdgrp lng="fr">
    <trd>dedans</trd>,
    <trd>&agrave; l'<ind>int&eacute;rieur</ind></trd>
  </trdgrp>
  <trd lng="he">&bet;&pe;&nun;&yod;&fmem;</trd>
  <trdgrp lng="pl">
    <trd>w<klr> &sacute;rodku</klr></trd>,
