    <trd lng="sl">v</trd>
  </snc>
  <snc mrk="en.0.Ak">
    <dif>
      Prepozicio; uzate kun akuzativo, indikas direkton per
      finpunkto, t.e. fizikan a&ubreve; abstraktan lokon, situacion a&ubreve;
      staton, internen de kiu io a&ubreve; iu movi&gcirc;as, celas, strebas,
      evoluas:
    </dif>
    <subsnc mrk="en.0.loka">
      <dif>
        (kun akuzativo:) Indikas lokon,
        <tld/><sncref ref="en.0.kie"/> kiu situas la celo a&ubreve;
        finpunkto:
        <ekz>
          la birdo flugas <tld/> la &ccirc;ambron (= &gcirc;i estas ekster la
          &ccirc;ambro kaj flugas nun <tld/> &gcirc;in)
          <fnt><bib>F</bib>, <lok>&FE;26.</lok></fnt>;
        </ekz>
        <ekz>
          vi povas iri <klr>[&#8230;]</klr> <tld/> teatron
          <fnt>
            <bib>IdO</bib>
            <lok>Unua Parto</lok>
          </fnt>;
        </ekz>
        <ekz>
          &leftquot;Vi devas nepre veturi <tld/> banlokon!&rightquot;
          diris la ombro
          <fnt><bib>Fab2</bib>, <lok>La ombro</lok></fnt>;
        </ekz>
        <ekz>
          la pastroj eniris <tld/> la internon de la domo
          <fnt>
            <bib>MT</bib>
            <lok>II. Kroniko 29:16</lok>
          </fnt>;
        </ekz>
        <ekz>
          morga&ubreve; mi veturos Parizon (a&ubreve; <tld/> Parizon)
          <fnt><bib>F</bib>, <lok>&FE;28.</lok></fnt>;
        </ekz>
        <ekz>
          anseroj diskuradis <tld/> &ccirc;iujn direktojn
          <fnt>
            <bib>Far1</bib>
            <lok>&Ccirc;apitro XI</lok>
          </fnt>;
        </ekz>
        <ekz>
          sablero enfalis <tld/> mian okulon
          <fnt>
            <bib>F</bib>
            <lok>&FE; 41</lok>
          </fnt>;
        </ekz>
        <ekz>
          tiufoje <klr>[&scirc;i]</klr> ne forgesis meti la &scirc;losilon <tld/> sian po&scirc;on
          <fnt>
            <bib>KPr</bib>
            <lok>&Ccirc;apitro Kvina</lok>
          </fnt>;
        </ekz>
        <ekz>
          <klr>[&scirc;i]</klr> enigis la manon <tld/> sian ledan sakon
          <fnt>
            <bib>Far1</bib>
            <lok>&Ccirc;apitro XIX</lok>
          </fnt>;
        </ekz>
        <ekz>
          <nom>Samuel</nom> <klr>[&#8230;]</klr> skribis tion <tld/> libron
          <fnt>
            <bib>MT</bib>
            <lok>I. Samuel 10:25</lok>
          </fnt>;
        </ekz>
        <ekz>
          mi malfermas la pordon <tld/> la dormo&ccirc;ambron
          <klr>(t.e. la pordon kiu kondukas <tld/> la
          dormo&ccirc;ambron, per kiu oni transiras <tld/> la
          dormo&ccirc;ambron)</klr>
          <fnt><bib>Metrop</bib></fnt>;
        </ekz>
        <ekz>
          e&ccirc; plej ruza vulpo <tld/> kaptilon falas
          <fnt><bib>PrV</bib></fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="al.0">al</ref>
      </refgrp>
      <trd lng="he">&lamed;&tav;&vav;&fkaf;</trd>
    </subsnc>
    <subsnc mrk="en.0.celstata">
      <dif>
        (kun akuzativo:) Indikas la finan staton, situacion ktp
        de evoluo, alii&gcirc;o, &scirc;an&gcirc;o:
        <ekz>
          la idolojn li disbatis <tld/> pecetojn
          <fnt>
            <bib>MT</bib>
            <lok>II. Kroniko 34:7</lok>
          </fnt>;
        </ekz>
        <ekz>
          montrante per unu mano la infanon kaj la duan manon kunpreminte <tld/> pugnon
          <fnt>
            <bib>FK</bib>
            <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
          </fnt>;
        </ekz>
        <ekz>
          traduki el unu <tld/> alian lingvon
          <fnt>
            <bib>DKM</bib>
            <lok>&Ccirc;apitro LXII</lok>
          </fnt>;
        </ekz>
        <ekz>
          la dek na&ubreve; provincoj, <tld/> kiujn &Hcirc;inujo estas dividita
          <fnt>
            <bib>FK</bib>
            <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
          </fnt>;
        </ekz>
        <ekz>
          la malharmonio aliformi&gcirc;as <tld/> harmonion
          <fnt>
            <bib>Fab3</bib>
            <lok>Dorna vojo de la honoro</lok>
          </fnt>;
        </ekz>
        <ekz>
          nia aristokrataro falis <tld/> mizeron
          <fnt>
            <bib>Far2</bib>
            <lok>&Ccirc;apitro IV</lok>
          </fnt>;
        </ekz>
        <ekz>
          <klr>[lia]</klr> malka&scirc;emeco <klr>[&#8230;]</klr> aliformi&gcirc;is <tld/>
          arogantecon
          <fnt>
            <bib>Rabistoj</bib>
            <lok>Akto Unua</lok>
          </fnt>;
        </ekz>
        <ekz>
          &scirc;iru vin <tld/> du partojn, la mondo trian postulos
          <fnt>
            <bib>PrV</bib>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="al.0">al</ref>,
        <ref cel="el.0.stata">el<sncref/></ref>
      </refgrp>
      <trd lng="he">&lamed;-</trd>
    </subsnc>
