<drv mrk="en.0ede">
  <kap><tld/>e de</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="en.0ede.kie">
    <dif>
      <ref tip="sin" cel="en.0.kie"><tld lit="E"/><sncref/></ref>,
      en la interno de, <ref cel="sin.0ede.kie">sine de</ref>:
      <ekz>
        <tld/>e de la arbaro &scirc;i renkontas tiun kiun &scirc;i pli
        amas ol iu ajn esta&jcirc;o
        <fnt>
          <aut>M. de France, trad. R. Rius</aut>,
          <vrk><url ref="http://www.angelfire.com/falcon/ramonrius/lit_marie%20de%20france.htm">
          La Lajo pri la Lonicero</url></vrk>,
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_u;</trd>,
      <trd>&c_u;&c_n;&c_u;&c_t;&c_r;&c_y;</trd>
    </trdgrp>
    <trd lng="de">im <ind>Inneren</ind> von</trd>
    <trd lng="es">en el interior de</trd>
    <trd lng="fr">&agrave; l'int&eacute;rieur de</trd>
    <trd lng="pl">wewn&aogonek;trz</trd>
  </snc>
  <snc mrk="en.0ede.kiam">
    <dif>
      <ref cel="dum.0.konj_temp">Dum</ref> tiu da&ubreve;ro, post &gcirc;ia komenco kaj
      anta&ubreve; &gcirc;ia fino:
      <ekz>
        la invado konkeris Irakon <tld/>e de unu monato
        <fnt>
          <bib>Kon11</bib>
        </fnt>;
      </ekz>
      <ekz>
        kontra&ubreve; disciplinaj decidoj de la akademia senato la
        koncernatoj povas protesti <tld/>e de 90 tagoj
        <fnt>
          <aut>AIS San-Marino</aut>,
          <vrk><url ref="http://www.ais-sanmarino.org/regular/tekstoj/ST.html">
          Statuto</url></vrk>,
          <lok>1986</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_d;&c_ch;&c_a;&c_s;</trd>,
      <trd>&c_z;&c_a; <ind>&c_p;&c_e;&c_r;&c_y;&c_ja;&c_d;</ind></trd>
    </trdgrp>
    <trd lng="de"><ind>innerhalb</ind> von</trd>
    <trd lng="es">en un plazo</trd>
    <trd lng="fr">dans un d&eacute;lai de</trd>
    <trdgrp lng="pl">
      <trd>w czasie <klr>(czego&sacute;)</klr></trd>,
      <trd>podczas <klr>(czego&sacute;)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="cs">uvnit&rcaron;</trd>
  <trd lng="ja">&#65374;<ind>&#12398;&#20869;&#37096;&#12391;  </ind><pr>&#12398;&#12394;&#12356;&#12406;&#12391;</pr></trd>
  <trd lng="sk">vn&uacute;tri</trd>
</drv>

