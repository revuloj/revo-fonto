<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>trirem</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="trirem.0o">
  <kap><tld/>o</kap>
  <snc mrk="trirem.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      <ref tip="super" cel="sxip.milit0o.MAR">Milit&scirc;ipo</ref> kun tri
      remilo-vicoj sur &ccirc;iu flanko, uzata antikve en Mediteraneo:
      <ekz>
        kablo &ccirc;irka&ubreve;is la cirkonferencon de <tld/>o por teni firma
la
        strukturon de la &scirc;ipo
        <fnt>
          <aut>Platono, trad. D. Broadribb</aut>,
          <vrk><url ref="http://www.luin.se/inko/X26-4.pdf">La
          Respubliko</url>,
[noto]</vrk>,
          <lok>1993-2000</lok>
        </fnt>;
      </ekz>
      <ekz>
        grandeguloj de &ccirc;irka&ubreve; duonstadia staturo alnavigis sur egaj
        insuloj kvaza&ubreve; sur <tld/>oj
        <fnt><aut>Lukiano, trad. G. Berveling</aut>, <vrk><url
        ref="http://www.esperanto.gr/lit/els/rakontoj.pdf">veraj
        rakontoj</url>. - 2005</vrk></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">trirrem</trd>
  <trdgrp lng="fr">
    <trd>tri&egrave;re</trd>,
    <trd>trir&egrave;me</trd>
  </trdgrp>
  <trd lng="sk">trirema</trd>
</drv>
</art>

