<kap>
  <rad>trirem</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="trirem.0o">
  <kap><tld/>o</kap>
  <snc mrk="trirem.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Antikva <ref tip="super" cel="sxip.milit0o.SHI">milit&scirc;ipo</ref> kun tri
      remilo-vicoj sur &ccirc;iu flanko, iam uzita en Mezmaro:
      <ekz>
        kablo &ccirc;irka&ubreve;is la cirkonferencon de <tld/>o por teni firma
        la strukturon de la &scirc;ipo
        <fnt>
          <aut>Platono, trad. D. Broadribb</aut>
          <vrk><url ref="http://www.luin.se/inko/X26-4.pdf">La
          Respubliko</url> [noto]</vrk>
          <lok>1993-2000</lok>
        </fnt>;
      </ekz>
      <ekz>
        grandeguloj de &ccirc;irka&ubreve; duonstadia staturo alnavigis sur egaj
        insuloj kvaza&ubreve; sur <tld/>oj
        <fnt>
          <aut>Lukiano, trad. G. Berveling</aut>
          <vrk><url ref="http://www.esperanto.gr/lit/els/rakontoj.pdf">
          Veraj rakontoj</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_y;&c_je;&c_r;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_y;&c_r;&c_e;&c_m;&c_a;</trd>
  </trdgrp>
  <trd lng="as">trirreme</trd>
  <trd lng="bg">&c_t;&c_r;&c_i;&c_je;&c_r;&c_a;</trd>
  <trd lng="br">teirroe&ntilde;veg</trd>
  <trd lng="bs">trirema</trd>
  <trd lng="ca">trirrem</trd>
  <trd lng="cs">trir&eacute;ma</trd>
  <trdgrp lng="de">
    <trd>Triere</trd>,
    <trd>Trireme</trd>
  </trdgrp>
  <trd lng="en">trireme</trd>
  <trd lng="es">trirreme</trd>
  <trdgrp lng="fi">
    <trd>kolmisoutu</trd>,
    <trd>trieeri</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>tri&egrave;re</trd>,
    <trd>trir&egrave;me</trd>
  </trdgrp>
  <trd lng="gl">trirreme</trd>
  <trd lng="grc">&tau;&rho;&jota;&eta_ton;&rho;&eta;&sigma_fina;</trd>
  <trd lng="he">&tav;&lamed;&tav; &het;&tav;&resh;&yod;&tav;</trd>
  <trd lng="hu">tri&eacute;r&eacute;sz</trd>
  <trd lng="io">triremo</trd>
  <trd lng="it">trireme</trd>
  <trd lng="ja">&#19977;&#27573;&#27330;&#33337; <pr>&#12373;&#12435;&#12384;&#12435;&#12363;&#12356;&#12379;&#12435;</pr></trd>
  <trd lng="ko">&#45432;&#51032; &#44400;&#49440;</trd>
  <trd lng="la">triremis</trd>
  <trd lng="lt">trirema</trd>
  <trd lng="nl">trireem</trd>
  <trd lng="no">trirem</trd>
  <trd lng="pl">trirema</trd>
  <trd lng="pt">trirreme</trd>
  <trd lng="ru">&c_t;&c_r;&c_i;&c_r;&c_je;&c_m;&c_a;</trd>
  <trd lng="sk">trirema</trd>
  <trd lng="sl">triera</trd>
  <trd lng="sr">&c_t;&c_r;&c_i;&#1112;&c_je;&c_r;&c_a;</trd>
  <trd lng="sv">triremer</trd>
  <trd lng="uk">&c_t;&c_r;&c_i;&c_jeu;&c_r;&c_a;</trd>
  <trd lng="zh">&#19977;&#21015;&#27123;&#24231;&#25136;&#33337;</trd>
</drv>
