<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <rad>dekrement</rad>/o
</kap>

<drv mrk="dekrem.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Nombro, kiu karakterizas la amortizon de osciloj.
    </dif>
    <adm>
      La franca konas alian sencon, kiu eble ekzistas anka&ubreve; en
      Esperanto, nome : malgrandi&gcirc;o de variablo. <aut>MB</aut>
    </adm>
  </snc>
  <trd lng="ca">decrement</trd>
  <trd lng="de">Dekrement</trd>
  <trd lng="es">decremento</trd>
  <trd lng="fr">d&eacute;cr&eacute;ment</trd>
  <trd lng="hu">dekrementum</trd>
  <trdgrp lng="pt">
    <trd>decremento</trd>,
    <trd>decrescimento</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_je;&c_k;&c_r;&c_je;&c_m;&c_je;&c_n;&c_t; &c_z;&c_a;&c_t;&c_u;&c_h;&c_a;&c_n;&c_i;&c_ja;</trd>
</drv>

<drv mrk="dekrem.logaritma0o">
  <kap>logaritma <tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Logaritmo de la rilato de la amplitudo de unu oscilo al la
      sekvanta
      oscilo.
    </dif>
  </snc>
  <trd lng="ca"><ind>decrement</ind> logar&iacute;tmic</trd>
  <trd lng="de">logarithmisches <ind>Dekrement</ind></trd>
  <trd lng="es"><ind>decremento</ind> logar&iacute;tmico</trd>
  <trd lng="fr">d&eacute;cr&eacute;ment logarithmique</trd>
  <trd lng="hu">logaritmikus <ind>dekrementum</ind></trd>
  <trd lng="ru">&c_l;&c_o;&c_g;&c_a;&c_r;&c_i;&c_f;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; <ind>&c_d;&c_je;&c_k;&c_r;&c_je;&c_m;&c_je;&c_n;&c_t;</ind> &c_z;&c_a;&c_t;&c_u;&c_h;&c_a;&c_n;&c_i;&c_ja;</trd>
</drv>
</art>
