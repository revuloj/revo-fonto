    <trd>abbauen</trd>,
    <trd>abmontieren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>dismantle</trd>,
    <trd>disassemble</trd>,
    <trd>take apart</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;monter</trd>,
    <trd>d&eacute;sassembler<klr> (d&eacute;monter)</klr></trd>,
    <trd>disjoindre</trd>
  </trdgrp>
  <trd lng="hu">sz&eacute;tszerel</trd>
  <trdgrp lng="ja">
    <trd>&#35299;&#20307;&#12377;&#12427;  <pr>&#12363;&#12356;&#12383;&#12356;&#12377;&#12427;</pr></trd>,
    <trd>&#20998;&#35299;&#12377;&#12427;  <pr>&#12406;&#12435;&#12363;&#12356;&#12377;&#12427;</pr></trd>,
    <trd>&#21462;&#12426;&#22806;&#12377;  <pr>&#12392;&#12426;&#12399;&#12378;&#12377;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>demonteren</trd>,
    <trd>losmaken</trd>,
    <trd>ontmantelen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rozmontowa&cacute;</trd>,
    <trd>demontowa&cacute;</trd>,
    <trd>rozk&lstroke;ada&cacute;</trd>,
    <trd>rozbiera&cacute; <klr tip="amb">na cz&eogonek;&sacute;ci</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_o;&c_b;&c_r;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_je;&c_m;&c_o;&c_n;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">rozobra&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_je;&c_m;&c_o;&c_n;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_b;&c_i;&c_r;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_gu;&c_v;&c_i;&c_n;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#25286;&#38500; <pr>ch&amacron;ich&uacute;</pr></trd>,
