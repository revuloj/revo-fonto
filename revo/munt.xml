<drv mrk="munt.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="munt.0isto.TEK">
  <uzo tip="fak">TEK</uzo>
    <dif>
      Laboristo, kiu prizorgas la <tld/>adon:
      <ekz>
         li plene retiri&gcirc;is de la &jcirc;azo, laboris en Novjorko kiel
         elektro<tld/>isto
         <fnt>
           <bib>Viki</bib>,
           <lok>
             <url ref="&Viki;/Paul_Quinichette">
             Paul Quinichette</url>,
             2012-08-24
           </lok>
         </fnt>;
      </ekz>
      <ekz>
         anka&ubreve; multe lerni en mia nova profesio (tiu de
         elektro<tld/>isto-riparisto pri alt-tensiaj instala&jcirc;oj) mi
         devas por ne perdi laborlokon.<fnt>
           <lok>
             <url ref="http://my.opera.com/Samideano/blog/index.dml/tag/esperarto">
             Iom pri mia rareco (enreta)</url>,
             2012-08-24
           </lok>
         </fnt>;
      </ekz>
      <ekz>
        profesioj de &ccirc;inaj laborantoj &ccirc;efe estas konstruisto, kuiristo, tajloro,
        servistino kaj elektronika <tld/>isto
        <fnt>
          <aut>MU Binghua</aut>
            <vrk><url ref="&Monato;/2000/007046.php">Eksporto de laboristoj</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 10</lok>
          </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Monteur</trd>
    <trdgrp lng="zh">
      <trd>&#23433;&#35013;&#24037;&#20154; <pr>&amacron;nzhu&amacron;ngg&omacron;ngr&eacute;n</pr></trd>,
      <trd>&#23433;&#35037;&#24037;&#20154; <pr>&amacron;nzhu&amacron;ngg&omacron;ngr&eacute;n</pr></trd>,
      <trd>&#35013;&#37197;&#24037; <pr>zhu&amacron;ngp&egrave;ig&omacron;ng</pr></trd>,
      <trd>&#35037;&#37197;&#24037; <pr>zhu&amacron;ngp&egrave;ig&omacron;ng</pr></trd>,
      <trd>&#35013;&#37197;&#24037;&#20154; <pr>zhu&amacron;ngp&egrave;ig&omacron;ngr&eacute;n</pr></trd>,
      <trd>&#35037;&#37197;&#24037;&#20154; <pr>zhu&amacron;ngp&egrave;ig&omacron;ngr&eacute;n</pr></trd>,
      <trd>&#35013;&#37197;&#25216;&#24072; <pr>zhu&amacron;ngp&egrave;ij&igrave;sh&imacron;</pr></trd>,
      <trd>&#35037;&#37197;&#25216;&#24107; <pr>zhu&amacron;ngp&egrave;ij&igrave;sh&imacron;</pr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_n;&c_t;&c_a;&c_zh;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_m;&c_a;&c_n;&c_mol;&c_c;&c_jo;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mont&aacute;&zcaron;n&iacute;k</trd>,
    <trd>mont&eacute;r</trd>
  </trdgrp>
  <trd lng="fr">monteur</trd>
  <trd lng="pl">monter</trd>
  <trd lng="sk">mont&eacute;r</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_o;&c_n;&c_t;&c_je;&c_r;</trd>,
    <trd>&c_m;&c_o;&c_n;&c_t;&c_a;&c_zh;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_m;&c_o;&c_n;&c_t;&c_a;&c_zh;&c_je;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="munt.en0i">
