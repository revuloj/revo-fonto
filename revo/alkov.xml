<art mrk="$Id: alkov.xml,v 1.24 2023/11/23 21:21:39 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>alkov</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="alkov.0o">
  <kap><tld/>o</kap>
  <snc mrk="alkov.0o.litnicxo">
    <dif>
      En&ccirc;ambra litni&ccirc;o, iafoje fermebla per kurtenoj
      a&ubreve; pordo:
      <ekz>
        malanta&ubreve; &scirc;i, en la <tld/>o, mi rigardis &scirc;ian liton
        <fnt>
          <aut>G. de Maupassant, trad. Daniel Luez</aut>,
          <vrk><url ref="&LOdE;Tl-fr03.htm">
          La mastrino</url></vrk>,
          <lok>La Ondo de Esperanto, 1999:1 (51)</lok>
        </fnt>;
      </ekz>
      <ekz>
        same Generalon &parsl; Pli ofte libroj vidis, ol li hundoj-stalon, &parsl; Pli virinajn <tld/>ojn li,
        ol lin pel&ccirc;aso
        <fnt>
          <bib>SrT</bib>
          <lok>Libro VIII.</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ar">&a_A;&a_l;&a_q;&a_b;&a_t_marbuta;</trd>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_k;&c_o;&c_w;</trd>
  <trdgrp lng="cs">
    <trd>alkovna</trd>,
    <trd>p&rcaron;&iacute;st&ecaron;nek</trd>,
    <trd>v&yacute;klenek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Alkoven</trd>,
    <trd>Bettnische</trd>
  </trdgrp>
  <trd lng="en">alcove</trd>
  <trd lng="es">alcoba</trd>
  <trd lng="fr">alc&ocirc;ve</trd>
  <trd lng="he">&gimel;&vav;&#1468;&mem;&het;&#1464;&he;</trd>
  <trd lng="ja">&#12450;&#12523;&#12467;&#12540;&#12502;</trd>
  <trd lng="nl">alkoof</trd>
  <trd lng="pl">alkowa</trd>
  <trd lng="pt">alcova</trd>
  <trd lng="ru">&c_a;&c_l;&c_mol;&c_k;&c_o;&c_v;</trd>
  <trdgrp lng="sk">
    <trd>alkov&ncaron;a</trd>,
    <trd>l&ocirc;&zcaron;kov&yacute; v&yacute;klenok v izbe</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&c_l;&c_mol;&c_k;&c_o;&c_v;</trd>,
    <trd>&c_n;&c_ib;&c_sh;&c_a;</trd>
  </trdgrp>
</drv>
</art>
