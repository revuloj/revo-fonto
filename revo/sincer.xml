<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sincer.xml,v 1.24 2022/07/15 05:48:51 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sincer</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sincer.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="sincer.0a.iu">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Esprimanta <ref cel="kasx.mal0a.lojala">malka&scirc;e</ref>, senmensoge 
      kaj mal<ref tip="ant" cel="hipokr.0a">hipokrite</ref> tion, kion li
      a&ubreve; &scirc;i opinias a&ubreve; sentas; aganta akorde kun siaj opinioj kaj
      sentoj; <ref tip="sin" cel="ver.0ema">verema</ref>:
      <ekz>
        mi povus &jcirc;uri al vi, ke ili estas <tld/>aj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a kaj profunde konvinkita kristano
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Pri la homaranismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        el <tld/>a koro mi oferis &ccirc;ion &ccirc;i tion
        <fnt>
          <bib>MT</bib>
          <lok>I. Kroniko 29:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        pri &ccirc;io tio &ccirc;i mi estas <tld/>e kaj forte konvinkita
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        estu <tld/>aj kontra&ubreve; mi, &ccirc;u oni invitis vin a&ubreve; ne
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>?
      </ekz>
      <ekz>
        mia instruisto <klr>[estis]</klr> la plej <tld/>a kaj modesta kaj samtempe la
        plej klera vireto, kiun oni iam renkontis
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="brav.0a.honesta">brava<sncref/></ref>,
      <ref cel="fid.0inda">fidinda</ref>,
      <ref cel="kred.0inda">kredinda</ref>,
      <ref cel="lojal.0a">lojala</ref>,
      <ref cel="ferm.mal0ita.PSI">malfermita<sncref/></ref>,
      <ref cel="solid.0a.fidinda">solida<sncref/></ref>
    </refgrp>
  </snc>
  <snc mrk="sincer.0a.io">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Malka&scirc;e, senmensoge kaj malhipokrite dirita, farita a&ubreve;
      sentita; <ref tip="vid" cel="ver.0a">vera</ref>:
      <ekz>
        akceptu mian <tld/>an saluton
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[kun]</klr> <tld/>a estimo kaj koreco <klr>[&#8230;]</klr> mi diris &leftquot;bonan
        nokton&rightquot; al tiu &ccirc;i feli&ccirc;a paro
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni sentas kun kredo <tld/>a, &parsl; ke estos esper' plenumita
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kanto <tld/>a de mia animo, &parsl; flugu en vaston sen timo
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj okuloj duone kun raviteco, duone kun <tld/>a kompato estis fiksitaj sur la
        viza&gcirc;o de la juna vidvino
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        ilia &ccirc;agreno, &scirc;ajnas, estis <tld/>a
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        donaco ne<tld/>a estas donaco infera
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dec.0a">deca</ref>,
      <ref cel="honest.0a">honesta</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_ch;&c_y;&c_r;&c_y;</trd>
  <trd lng="bg">&c_i;&c_s;&c_k;&c_r;&c_je;&c_n;</trd>
  <trdgrp lng="ca">
    <trd>sincer</trd>,
    <trd>franc</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>up&rcaron;&iacute;mn&yacute;</trd>,
    <trd>up&rcaron;&iacute;mn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>aufrichtig</trd>,
    <trd>lauter</trd>
  </trdgrp>
  <trd lng="en">sincere</trd>
  <trdgrp lng="es">
    <trd>sincero</trd>,
    <trd>franco</trd>
  </trdgrp>
  <trd lng="fr">sinc&egrave;re</trd>
  <trd lng="hu">&odblac;szinte</trd>
  <trdgrp lng="nl">
    <trd>oprecht</trd>,
    <trd>openhartig</trd>
  </trdgrp>
  <trd lng="pl">szczery</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_k;&c_r;&c_je;&c_n;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_ch;&c_i;&c_s;&c_t;&c_o;&c_s;&c_je;&c_r;&c_d;&c_je;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nezaobalen&yacute;</trd>,
    <trd>otvoren&yacute;</trd>,
    <trd>&uacute;primn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_shch;&c_i;&c_r;&c_i;&c_j;</trd>,
    <trd>&c_shch;&c_i;&c_r;&c_o;&c_s;&c_je;&c_r;&c_d;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="sincer.0o">
  <kap><tld/>o,
  <var><kap><tld/>eco</kap></var></kap>
  <snc mrk="sincer.0o.PSI">
    <dif>
      <ref tip="vid" cel="dir.ver0emo.PSI">Verdiremo</ref>,
      inklino al malka&scirc;a diro de sentoj kaj opinioj:
      <ekz>
        la krabo &ccirc;iam montras <tld/>econ kaj amikecon al la 
        serpento, dum tiu estas insida kaj malvirta
        <fnt>
          <aut>red. Vejdo</aut>,
          <vrk><url ref="http://cindymckee.com/librejo/440FablojdeEzopo.pdf">
          440 Fabloj de Ezopo</url></vrk>,
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_ch;&c_y;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="cs">up&rcaron;&iacute;mnost</trd>
  <trd lng="fr">sinc&eacute;rit&eacute;</trd>
  <trd lng="sk">&uacute;primnos&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_shch;&c_i;&c_r;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_shch;&c_i;&c_r;&c_o;&c_s;&c_je;&c_r;&c_d;&c_ib;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="sincer.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="sincer.mal0a.iu">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      <ref tip="vid" cel="hipokr.0a">Hipokrita</ref>,
      mensogema, malverema:
      <ekz>
        mal<tld/>an bu&scirc;on mi malamas<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="ca">hip&ograve;crita</trd>
    <trd lng="es">hip&oacute;crita</trd>
  </snc>
  <snc mrk="sincer.mal0a.io">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Ne konforma al la vero,
      <ref tip="vid" cel="tromp.0a.KOMUNE">trompa</ref>.
    </dif>
    <trd lng="ca">falsejat</trd>
    <trd lng="es">falseado</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_sh;&c_ch;&c_y;&c_r;&c_y;</trd>,
    <trd>&c_k;&c_r;&c_y;&c_v;&c_a;&c_d;&c_u;&c_sh;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_n;&c_je;&c_i;&c_s;&c_k;&c_r;&c_je;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>neup&rcaron;&iacute;mn&yacute;</trd>,
    <trd>z&aacute;ludn&yacute;</trd>
  </trdgrp>
  <trd lng="de">unaufrichtig</trd>
  <trd lng="en">insincere</trd>
  <trdgrp lng="fr">
    <trd>hypocrite<klr> (adj.)</klr></trd>,
    <trd>insinc&egrave;re</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>onoprecht</trd>,
    <trd>achterbaks</trd>
  </trdgrp>
  <trd lng="pl">nieszczery</trd>
  <trdgrp lng="sk">
    <trd>falo&scaron;n&yacute;</trd>,
    <trd>ne&uacute;primn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_t;&c_a;&c_j;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_i;&c_t;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_shch;&c_i;&c_r;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_a;&c_z;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_i;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="sincer.mal0o">
  <kap>mal<tld/>o,
  <var><kap>mal<tld/>eco</kap></var></kap>
  <snc mrk="sincer.mal0o.PSI">
    <dif>
      Neverdiremo, trompa &scirc;ajnigo de aliaj sentoj
      kaj intencoj, ol oni reale havas:
      <ekz>
        estas malfacile trovi, geamikoj, pli <tld/>an
        konfeson de mal<tld/>eco
        <fnt>
          <aut>I. Lapenna</aut>,
          <vrk><url ref="https://www.youtube.com/watch?v=ZN-MZQEq8xs">
          [Prelego pri la komunismo kaj Stalin]</url></vrk>,
          <lok>[1986]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_sh;&c_ch;&c_y;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_r;&c_y;&c_v;&c_a;&c_d;&c_u;&c_sh;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="fr">hypocrisie</trd>
</drv>
</art>
