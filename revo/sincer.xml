<drv mrk="sincer.0o">
  <kap><tld/>o,
  <var><kap><tld/>eco</kap></var></kap>
  <snc mrk="sincer.0o.PSI">
    <dif>
      <ref tip="vid" cel="dir.ver0emo.PSI">Verdiremo</ref>,
      inklino al malka&scirc;a diro de sentoj kaj opinioj:
      <ekz>
        kie estas <tld/>eco, tie estas anka&ubreve; kompato kaj helpo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[t]</klr>ia honesta, estiminda <tld/>eco <klr>[&#8230;]</klr> havis por mi
        apartan &ccirc;armon
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li dankis lin pro lia kunsento, la&ubreve;dis lin pro liaj <tld/>eco kaj malka&scirc;emo
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        permesu, ke mi demandu vin, kiel patro, kun la tuta <tld/>eco: kian opinion vi
        havas pri mia filo
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XXI</lok>
        </fnt>?
      </ekz>
      <ekz>
        poemoj, potencaj per <tld/>o
        <fnt>
          <bib>VivZam</bib>
          <lok>Vivo de Zamenhof</lok>
        </fnt>;
      </ekz>
      <ekz>
        la krabo &ccirc;iam montras <tld/>econ kaj amikecon al la
        serpento, dum tiu estas insida kaj malvirta
        <fnt>
          <aut>red. Vejdo</aut>,
          <vrk><url ref="http://cindymckee.com/librejo/440FablojdeEzopo.pdf">
          440 Fabloj de Ezopo</url></vrk>,
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e tro &gcirc;entila ekstero mankas <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_ch;&c_y;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="cs">up&rcaron;&iacute;mnost</trd>
  <trdgrp lng="de">
    <trd>Aufrichtigkeit</trd>,
    <trd>Lauterkeit</trd>
  </trdgrp>
  <trd lng="fr">sinc&eacute;rit&eacute;</trd>
  <trdgrp lng="id">
    <trd><baz>tulus</baz>ketulusan</trd>,
    <trd><baz>ikhlas</baz>keikhlasan</trd>,
    <trd><baz>jujur</baz>kejujuran</trd>,
    <trd><baz>bersih hati</baz>kebersihan hati</trd>,
    <trd><baz>sungguh</baz>kesungguhan hati</trd>,
    <trd><baz>lurus hati</baz>kelurusan hati</trd>,
    <trd><baz>asli</baz>keaslian</trd>,
    <trd><baz>murni</baz>kemurnian</trd>,
    <trd><baz>tulen</baz>ketulenan</trd>,
    <trd><baz>jati</baz>kesejatian</trd>,
    <trd><baz>sungguh</baz>kesungguhan</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8AA0;&#x5B9F;  <pr>&#x305B;&#12356;&#12376;&#12388;</pr></trd>,
    <trd>&#x8AA0;&#x610F;  <pr>&#x305B;&#12356;&#12356;</pr></trd>,
    <trd>&#x771F;&#x5FC3;  <pr>&#x307E;&#12372;&#12371;&#x308D;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>prostota</trd>,
    <trd>rzetelno&sacute;&cacute;</trd>
  </trdgrp>
  <trd lng="sk">&uacute;primnos&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_shch;&c_i;&c_r;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_shch;&c_i;&c_r;&c_o;&c_s;&c_je;&c_r;&c_d;&c_ib;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x771F;&#x631A; <pr>zh&emacron;nzh&#xEC;</pr></trd>,
    <trd>&#x771F;&#x646F; <pr>zh&emacron;nzh&#xEC;</pr></trd>,
    <trd>&#x771F;&#x8AA0; <pr>zh&emacron;nch&#xE9;ng</pr></trd>,
    <trd>&#x771F;&#x8BDA; <pr>zh&emacron;nch&#xE9;ng</pr></trd>,
    <trd>&#x5FF1; <pr>ch&#xE9;n</pr></trd>,
    <trd>&#x771F;&#x631A; <pr>zh&emacron;nzh&#xEC;</pr></trd>,
    <trd>&#x771F;&#x646F; <pr>zh&emacron;nzh&#xEC;</pr></trd>,
    <trd>&#x771F;&#x8AA0; <pr>zh&emacron;nch&#xE9;ng</pr></trd>,
    <trd>&#x771F;&#x8BDA; <pr>zh&emacron;nch&#xE9;ng</pr></trd>,
    <trd>&#x5FF1; <pr>ch&#xE9;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sincer.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="sincer.mal0a.iu">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      <ref tip="vid" cel="hipokr.0a">Hipokrita</ref>,
      <ref cel="mensog.0ema">mensogema</ref>, malverema:
      <ekz>
        la mal<tld/>aj fratinoj volis per flato akiri la favoron de la juna re&gcirc;ino
        <fnt>
          <bib>ElektFab</bib>
          <lok>Cindrulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi ne vidas iliajn mal<tld/>ajn rigardojn <klr>[&#8230;]</klr> verdajn de avideco
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>an bu&scirc;on mi malamas
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 8:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ajna sed mal<tld/>a nedeziremo
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiso mal<tld/>a estas dan&gcirc;era
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">hip&ograve;crita</trd>
    <trd lng="es">hip&oacute;crita</trd>
  <trdgrp lng="id">
    <trd>munafik</trd>,
    <trd>hipokrit</trd>,
    <trd><baz>muka</baz>bermuka dua</trd>,
    <trd>serong hati</trd>
  </trdgrp>
  </snc>
  <snc mrk="sincer.mal0a.io">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Ne konforma al la vero,
      <ref tip="vid" cel="tromp.0a.KOMUNE">trompa</ref>.
    </dif>
    <trd lng="ca">falsejat</trd>
    <trd lng="es">falseado</trd>
  <trdgrp lng="id">
    <trd>palsu</trd>,
    <trd>lancung</trd>
  </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_sh;&c_ch;&c_y;&c_r;&c_y;</trd>,
    <trd>&c_k;&c_r;&c_y;&c_v;&c_a;&c_d;&c_u;&c_sh;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_n;&c_je;&c_i;&c_s;&c_k;&c_r;&c_je;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>neup&rcaron;&iacute;mn&yacute;</trd>,
    <trd>z&aacute;ludn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>unaufrichtig</trd>,
    <trd>falsch <klr>(unaufrichtig)</klr></trd>
  </trdgrp>
  <trd lng="en">insincere</trd>
  <trdgrp lng="fr">
    <trd>hypocrite<klr> (adj.)</klr></trd>,
    <trd>insinc&egrave;re</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4E0D;&#x8AA0;&#x5B9F;&#x306A;  <pr>&#12405;&#x305B;&#12356;&#12376;&#12388;&#x306A;</pr></trd>,
    <trd>&#33145;&#x9ED2;&#12356;  <pr>&#x306F;&#12425;&#12368;&#x308D;&#12356;</pr></trd>,
    <trd>&#x53E3;&#20808;&#12384;&#12369;&#x306E;  <pr>&#x304F;&#12385;&#12373;&#x304D;&#12384;&#12369;&#x306E;</pr></trd>,
    <trd>&#x507D;&#x308A;&#x306E;  <pr>&#12356;&#12388;&#x308F;&#x308A;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>onoprecht</trd>,
    <trd>achterbaks</trd>
  </trdgrp>
  <trd lng="pl">nieszczery</trd>
  <trdgrp lng="sk">
    <trd>falo&scaron;n&yacute;</trd>,
    <trd>ne&uacute;primn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_t;&c_a;&c_j;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_i;&c_t;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_shch;&c_i;&c_r;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_a;&c_z;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_i;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="sincer.mal0o">
  <kap>mal<tld/>o,
  <var><kap>mal<tld/>eco</kap></var></kap>
  <snc mrk="sincer.mal0o.PSI">
    <dif>
      Neverdiremo, trompa &scirc;ajnigo de aliaj sentoj
      kaj intencoj, ol oni reale havas:
      <ekz>
        eble <klr>[&#8230;]</klr> <nom>Sara</nom> ka&scirc;as mal<tld/>econ sub la &scirc;ajna boneco
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        estas malfacile trovi, geamikoj, pli <tld/>an
        konfeson de mal<tld/>eco
        <fnt>
          <aut>I. Lapenna</aut>,
          <vrk><url ref="https://www.youtube.com/watch?v=ZN-MZQEq8xs">
          [Prelego pri la komunismo kaj Stalin]</url></vrk>,
          <lok>[1986]</lok>
        </fnt>;
      </ekz>
      <ekz>
