    <trd>&#29575;&#x76F4;&#x306A;  <pr>&#x305D;&#12387;&#12385;&#12423;&#x304F;&#x306A;</pr></trd>,
    <trd>&#x771F;&#x5FC3;&#x306E;&#12371;&#12418;&#12387;&#x305F;  <pr>&#x307E;&#12372;&#12371;&#x308D;&#x306E;&#12371;&#12418;&#12387;&#x305F;</pr></trd>,
    <trd>&#x672C;&#x5FC3;&#x304B;&#12425;&#x306E;  <pr>&#x307B;&#12435;&#12375;&#12435;&#x304B;&#12425;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
