<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ekzemp1.xml,v 1.33 2025/11/01 20:28:26 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>ekzempler</rad>/o
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="ekzemp1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;iu el samspecaj a&jcirc;oj, konformaj al komuna tipo:
      <ekz>
         <uzo tip="fak">TIP</uzo>
         <ind><tld/>o de libro</ind>, de gazeto;
         <trdgrp lng="de">
           <trd>Buchausgabe</trd>
         </trdgrp>
         <trd lng="nl">proefdruk</trd>
      </ekz>
      <ekz>
         <uzo tip="fak">ZOO</uzo>
         malofta <ind><tld/>o de konko</ind>;
      </ekz>
      <ekz>
        tio estis lignogravuro <klr>[&#x2026;]</klr>, li hazarde trovis tian malnovan <tld/>on
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi povas ricevi de ni la bro&scirc;uron <klr>[&#x2026;]</klr> por la prezo de unu rublo por
        20 <tld/>oj
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>El la mondo homarana (hilelista).</vrk>
        </fnt>;
      </ekz>
      <ekz>
        sufi&ccirc;as unu homa <tld/>o por ju&gcirc;i pri &ccirc;iuj aliaj
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam &ccirc;i tiu sinjoro <klr>[&#x2026;]</klr> ekvidas belan a&ubreve; e&ccirc; beletan <tld/>on de
        virina viza&gcirc;o, li &jcirc;uras al &ccirc;iuj <klr>[&#x2026;]</klr>, ke li estas furioze, morte
        enami&gcirc;inta
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
         &ccirc;iu kliento funkciigas propran <tld/>on de la programo en
         sia komputilo, kaj ne laborigas la retservilon
         <fnt><aut>Klivo</aut>:
         <vrk>Kio estas Javo?</vrk>, <bib>Monato</bib><lok>jaro 1998a,
         numero 5a, p. 13a</lok></fnt>.
      </ekz>
    </dif>
     <refgrp tip="vid">
       <ref cel="ekzemp.0o">ekzemplo</ref>,
       <ref cel="specim.0o">specimeno</ref>,
       <ref cel="tip.0o">tipo</ref>
     </refgrp>
     <adm>
       Aldoni ekzemplon en botanika kunteksto.
       Kia rilato kun "specimeno", "tipo"? <aut>MB</aut>
     </adm>
   </snc>
  <trdgrp lng="be">
     <trd>&c_e;&c_k;&c_z;&c_e;&c_m;&c_p;&c_l;&c_ja;&c_r;</trd>,
     <trd>&c_a;&c_s;&c_o;&c_b;&c_n;&c_ib;&c_k;</trd>
   </trdgrp>
  <trd lng="ca">exemplar</trd>
  <trdgrp lng="cs">
    <trd>exempl&aacute;&rcaron;</trd>,
    <trd>jedinec</trd>,
    <trd>jednotlivina</trd>,
    <trd>kus</trd>,
    <trd>stejnopis</trd>,
    <trd>v&yacute;tisk</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Exemplar</trd>,
    <trd>Vertreter <klr>(einer Art)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>copy</trd>,
     <trd>specimen</trd>
   </trdgrp>
  <trd lng="es">ejemplar</trd>
  <trd lng="fa">&a_n;&a_s;&a_kh;&a_h;</trd>
  <trd lng="fr">exemplaire</trd>
  <trd lng="hu">p&eacute;ld&aacute;ny</trd>
  <trdgrp lng="ja">
    <trd>&#x4E00;&#x518A;  <pr>&#x3044;&#x3063;&#x3055;&#x3064;</pr></trd>,
    <trd>&#x4E00;&#x90E8;  <pr>&#x3044;&#x3061;&#x3076;</pr></trd>,
    <trd>&#x500B;&#x4F53;  <pr>&#x3053;&#x305F;&#x3044;</pr></trd>,
    <trd>&#x6A19;&#x672C;  <pr>&#x3072;&#x3087;&#x3046;&#x307B;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">exemplaar</trd>
  <trdgrp lng="pl">
     <trd>egzemplarz</trd>,
     <trd>okaz</trd>
   </trdgrp>
  <trdgrp lng="pt">
     <trd>exemplar</trd>,
     <trd>c&oacute;pia</trd>
   </trdgrp>
  <trd lng="ru">&c_e;&c_k;&c_z;&c_je;&c_m;&c_p;&c_l;&c_ja;&c_r;</trd>
  <trd lng="sk">exempl&aacute;r</trd>
  <trd lng="sv">exemplar</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_i;&c_m;&c_ib;&c_r;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_je;&c_k;&c_z;&c_je;&c_m;&c_p;&c_l;&c_ja;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5355;&#x672C; <pr>d&amacron;nb&ecaron;n</pr></trd>,
    <trd>&#x4EFD; <pr>f&egrave;n</pr></trd>,
    <trd>&#x518A; <pr>c&egrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ekzemp1.prov0o">
   <kap>prov<tld/>o</kap>
   <snc>
     <dif>
       <tld lit="E"/>o de komerca varo, speciale destinita por tio,
       ke oni povu <ref cel="prov.0i.kontroli_tauxgecon">provi
       <sncref/></ref>
       la varon kaj pritaksi &gcirc;ian kvaliton:
       <ekz>
         kiam la presejo sendis al la direktoro prov<tld/>on
         <klr>[de la verko]</klr>, li estis terurita
         <fnt>
         <vrk>LOdE</vrk>
         <lok>
         <url ref="&LOdE;76-lode.htm">2001:2</url></lok></fnt>;
       </ekz>
       <ekz>
         en dek tagoj li fabrikis prov<tld/>on de
         <klr>[koton-pritrakta]</klr>
         ma&scirc;ino, per kiu unu homo faras la laboron de 50
         <fnt><vrk>La Gazeto</vrk><lok>
         <url ref="http://www.osiek.org/lagazeto/LG-50/kotono-mashino.html">
         N-ro 50</url>
         </lok></fnt>.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="numer.prov0o">provnumero</ref>,
       <ref cel="protot.0o">prototipo</ref>,
       <ref cel="specim.0o">specimeno</ref>
     </refgrp>
   </snc>
   <trdgrp lng="be">
     <trd>&c_p;&c_r;&c_o;&c_b;&c_n;&c_ib;&c_k;</trd>,
     <trd>&c_p;&c_r;&c_o;&c_b;&c_n;&c_y; &c_a;&c_s;&c_o;&c_b;&c_n;&c_ib;&c_k;</trd>,
     <trd>&c_u;&c_z;&c_o;&c_r;</trd>
   </trdgrp>
  <trd lng="ca">exemplar de mostra</trd>
  <trd lng="cs">zku&scaron;ebn&iacute; vzorek</trd>
  <trd lng="de">Probeexemplar</trd>
  <trdgrp lng="en">
     <trd>trial copy</trd>,
     <trd>sample</trd>,
     <trd>specimen</trd>
   </trdgrp>
  <trdgrp lng="fa">
     <trd>&a_n;&a_m;&a_w;&a_n;&a_h;</trd>,
     <trd>&a_A;&a_kasra;&a_sh;&a_A;&a_n;&a_t;&f_y;&a_w;&a_n;</trd>
   </trdgrp>
  <trdgrp lng="fr">
     <trd>sp&eacute;cimen</trd>,
     <trd>&eacute;chantillon</trd>
   </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x898B;&#x672C;&#x53F7;  <pr>&#x307F;&#x307B;&#x3093;&#x3054;&#x3046;</pr></trd>,
    <trd>&#x898B;&#x672C;&#x672C;  <pr>&#x307F;&#x307B;&#x3093;&#x307B;&#x3093;</pr></trd>,
    <trd>&#x898B;&#x672C;&#x5237;&#x308A;  <pr>&#x307F;&#x307B;&#x3093;&#x3059;&#x308A;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
     <trd>proefexemplaar</trd>,
     <trd>specimen</trd>,
     <trd>voorbeeld</trd>
   </trdgrp>
  <trdgrp lng="pl">
     <trd>tester</trd>,
     <trd>pr&oacute;bka</trd>
   </trdgrp>
  <trd lng="sk">sk&uacute;&scaron;obn&aacute; vzorka</trd>
  <trd lng="uk">(&c_p;&c_r;&c_o;&c_b;&c_n;&c_i;&c_j;) &c_z;&c_r;&c_a;&c_z;&c_o;&c_k;</trd>
  <trd lng="zh">&#x68C0;&#x9A8C;&#x6837;&#x672C; <pr>ji&#x1CE;ny&agrave;ny&agrave;ngb&ecaron;n</pr></trd>
</drv>
