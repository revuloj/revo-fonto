<drv mrk="sam.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Io <tld/>a:
      <ekz>
        vi estas kapabla post unu monato diri la <tld/>on pri mi
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[mi]</klr> deziras al vi la <tld/>on, kion vi deziras al mi
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXV</lok>
        </fnt>;
      </ekz>
      <ekz>
        al ni la verda koloro diras la <tld/>on: iru, ne haltu, anta&ubreve;en kun kura&gcirc;o
        <fnt>
          <bib>Ret</bib>
          <lok>Aldono</lok>
        </fnt>!
      </ekz>
      <ekz>
        &ccirc;iu botanikisto posedas tiujn vortojn kaj multajn aliajn <klr>[pri sia
        fako]</klr>, la <tld/>o validas por zoologo, medicinisto, juristo
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">das <ind>Selbe</ind></trd>
  <trd lng="hu">ugyanaz <klr> (a dolog)</klr>&gt;</trd>
  <trd lng="ru">&c_t;&c_o; &c_zh;&c_je; &c_s;&c_a;&c_m;&c_o;&c_je;</trd>
</drv>









<drv mrk="sam.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      <ref cel="difere.0a">Diferenca</ref>:
      <ekz>
        la opinioj pri &gcirc;i estis mal<tld/>aj, sed la historio konservi&gcirc;is
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        reciproka kompreni&gcirc;ado inter personoj de mal<tld/>aj nacioj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista en 
          Boulogne sur Mer en la 5a de a&ubreve;gusto 1905</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la orkestroj en la fronto ludis kantojn, &ccirc;iun de mal<tld/>a takto
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        amba&ubreve; esta&jcirc;oj de mal<tld/>aj a&gcirc;oj tiel interkonformi&gcirc;is reciproke, ke <klr>[&#8230;]</klr>
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        prezenti du mal<tld/>ajn tipojn
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dektria</lok>
        </fnt>;
      </ekz>
      <ekz>
        paroloj kaj faroj estas mal<tld/>aj aferoj
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="ali.0a">alia</ref>,
      <ref cel="apart.0a">aparta</ref>,
      <ref cel="divers.0a">diversa</ref>,
      <ref cel="vari">varia</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_n;&c_sh;&c_y;</trd>,
    <trd>&c_a;&c_d;&c_r;&c_o;&c_z;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">rozd&iacute;ln&yacute;</trd>
  <trdgrp lng="de">
    <trd>ungleich</trd>,
    <trd>nicht dasselbe</trd>
  </trdgrp>
  <trd lng="en">different</trd>
  <trd lng="es">distinto</trd>
  <trd lng="fr">diff&eacute;rent</trd>
  <trdgrp lng="hu">
    <trd>teljesen m&aacute;s</trd>
    <trd>k&uuml;l&ouml;nb&ouml;z&odblac;</trd>,
    <trd>elt&eacute;r&odblac;</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>diverso</trd>,
    <trd>differente</trd>,
    <trd>distinto</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ongelijk</trd>,
    <trd>verschillend</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_r;&c_u;&c_g;&c_o;&c_j;</trd>,
    <trd>&c_i;&c_n;&c_o;&c_j;</trd>
  </trdgrp>
