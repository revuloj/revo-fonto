        <trd>identique</trd>,
        <trd>similaire</trd>,
        <trd>m&ecirc;me</trd>
      </trdgrp>
  <trd lng="fy">selde</trd>
  <trd lng="gl">mesmo</trd>
  <trd lng="gu">&#xA9C;</trd>
  <trd lng="ha">haka</trd>
  <trd lng="hi">&#x935;&#x939;&#x940;</trd>
  <trd lng="hr">isti</trd>
  <trd lng="hu">ugyanaz</trd>
  <trd lng="hy">&#x576;&#x578;&#x582;&#x575;&#x576;</trd>
  <trd lng="is">sama</trd>
  <trdgrp lng="it">
        <trd>stesso</trd>,
        <trd>uguale</trd>,
        <trd>medesimo</trd>
      </trdgrp>
  <trd lng="ja">&#x540C;&#x3058;</trd>
  <trd lng="jw">padha</trd>
  <trd lng="ka">&#x10D8;&#x10D2;&#x10D8;&#x10D5;&#x10D4;</trd>
  <trd lng="km">&#x178A;&#x17BC;&#x1785;&#x1782;&#x17D2;&#x1793;&#x17B6;</trd>
  <trd lng="ko">&#xB3D9;&#xC77C;</trd>
  <trd lng="lo">&#xE94;&#xEBD;&#xEA7;&#xE81;&#xEB1;&#xE99;</trd>
