    <drv mrk="sam.0a">
      <kap><ofc>*</ofc><tld/>a</kap>
      <snc mrk="sam.0a.tia">
        <dif>
          Estanta &ccirc;iel simila al alia; neniel distingebla de alia:
          <ekz>
            <ind>de <tld/>a koloro</ind>, de <tld/>a valoro
            <fnt><bib>PrV</bib></fnt>;
            <trd lng="hu">ugyanolyan sz&iacute;n&udblac;</trd>
          </ekz><ekz>
            el <tld/>a tero devenas, <tld/>an sukon entenas
            <fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            ne <tld/>a la vento blovas konstante
            <fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            <tld/>a gento, <tld/>a sento
            <fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            ni ne uzas la <tld/>an lingvon;
          </ekz><ekz>
            mi &gcirc;ojas, ke vi havas tian <tld/>an opinion, kiel mi;
          </ekz><ekz>
            en la <tld/>a tempo;
          </ekz><ekz>
            la uzado de la artikolo en Esperanto estas tia <tld/>a, kiel
            
en la
            aliaj lingvoj<fnt>Z</fnt>;
          </ekz><ekz>
            esence tio estas la <tld/>a teorio, kia tiu de Z;
          </ekz><ekz>
            tiu libro estas la <tld/>a kiel la mia;
          </ekz><ekz>
            <ind><tld/>speca</ind>;
            <trdgrp lng="hu">
              <trd>egyf&eacute;le</trd>,
              <trd>egyfajta</trd>,
              <trd>hasonsz&odblac;r&udblac;</trd>
            </trdgrp>
            <trd lng="nl">soortgelijk</trd>
            <trd lng="ru">&c_t;&c_o;&c_g;&c_o; &c_zh;&c_je; 
&c_r;&c_o;&c_d;&c_a;</trd>
          </ekz><ekz>
            <tld/>sorta;
          </ekz><ekz>
            <tld/>sangeco;
          </ekz><ekz>
            <ind><tld/>opinii</ind>;
            <trd lng="be">&c_p;&c_a;&c_g;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_c;&c_a;</trd>
            <trdgrp lng="hu">
              <trd>azonosan v&eacute;lekedik</trd>,
              <trd>egy a v&eacute;lem&eacute;nye</trd>
              <trd>ugyanaz a v&eacute;lem&eacute;nye</trd>
            </trdgrp>       
            <trd lng="nl">het <ind>eens</ind> zijn</trd>
            <trdgrp lng="ru">
              
<trd>&c_s;&c_o;&c_g;&c_l;&c_a;&c_sh;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
              <trd>&c_i;&c_m;&c_je;&c_t;&c_mol; &c_t;&c_o; &c_zh;&c_je;
&c_m;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
            </trdgrp>
          </ekz><ekz>
            <ind><tld/>tempulo</ind>;
            <trd lng="be">&c_s;&c_u;&c_ch;&c_a;&c_s;&c_mol;&c_n;&c_ib;&c_k;</trd>
            <trd lng="ca">contemporani</trd>
            <trd lng="es">contempor&aacute;neo</trd>
            <trd lng="hu">kort&aacute;rs</trd>       
            <trd lng="nl">tijdsgenoot</trd>
            <trd lng="ru">&c_s;&c_o;&c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_i;&c_k;</trd>
          </ekz><ekz>
            <tld/>tempe kiel iu;
          </ekz><ekz>
            <ind><tld/>tempe</ind> kun iu;
            <trdgrp lng="be">
              
<trd>&c_a;&c_d;&c_n;&c_a;&c_ch;&c_a;&c_s;&c_o;&c_v;&c_a;</trd>,
              <trd>&c_a;&c_d;&c_n;&c_a;&c_ch;&c_a;&c_s;&c_n;&c_a;</trd>
            </trdgrp>
            <trd lng="es">al <ind>mismo</ind> tiempo</trd>
            <trdgrp lng="hu">
              <trd>egy id&odblac;ben</trd>,
              <trd>ugyanakkor</trd>,
              <trd>egyszerre</trd>
            </trdgrp>
            <trd lng="ru">&c_o;&c_d;&c_n;&c_o;&c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_o;</trd>
          </ekz><ekz>
            la ar&hcirc;aismoj estos presataj <tld/>tempe kun la formoj
            novaj<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>regnanoj;
          </ekz><ekz>
            <ind><tld/>urbano</ind>j;
            <trd lng="nl">stadsgenoot</trd>
            <trdgrp lng="hu">
              <trd>egy v&aacute;rosb&oacute;l val&oacute;</trd>,
              <trd>v&aacute;rosbeli</trd>
            </trdgrp>
          </ekz><ekz>
            <ind><tld/>landano</ind>j;
            <trd lng="be">&c_s;&c_u;&c_a;&c_j;&c_ch;&c_y;&c_n;&c_mol;&c_n;&c_ib;&c_k;</trd>
            <trd lng="es">compatriota</trd>
            <trd lng="hu">honfit&aacute;rs</trd>
            <trd lng="nl">landgenoot</trd>
            <trd lng="ru">&c_s;&c_o;&c_o;&c_t;&c_je;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_i;&c_k;</trd>
          </ekz><ekz>
            <ind><tld/>religiano</ind>j.
            <trd lng="ca">correligionari</trd>
            <trd lng="es">correligionario</trd>
            <trdgrp lng="hu">
              <trd>hitsorsos</trd>,
              <trd>hittestv&eacute;r</trd>
            </trdgrp>
            <trd lng="nl">geloofsgenoot</trd>
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="simil.0a">simila</ref>,
          <ref cel="egal.0a">egala</ref>.
        </refgrp>
        <trdgrp lng="be">
          <trd>&c_t;&c_a;&c_k;&c_ib; &c_zh;</trd>,
          <trd>&c_t;&c_a;&c_k;&c_ib; &c_zh; &c_s;&c_a;&c_m;&c_y;</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>ugyanolyan</trd>,
          <trd>azonos</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_t;&c_a;&c_k;&c_o;&c_j; &c_zh;&c_je;</trd>,
          <trd>&c_t;&c_a;&c_k;&c_o;&c_j; &c_zh;&c_je; 
&c_s;&c_a;&c_m;&c_y;&c_j;</trd>
        </trdgrp>
      </snc>
      <snc mrk="sam.0a.tiu">
        <dif>
          Estanta nedistingebla de iu a&ubreve; io alia, pro tio, ke 
&gcirc;i estas
          &ccirc;i tiu alia mem;
          <ref tip="sin" cel="ident.0a.tiu_sama">identa<sncref/></ref>:
          <ekz>
            tio &ccirc;i estis tiu <tld/>a feino, kiu prenis sur sin la 
formon de
            princino<fnt>Z</fnt>;
          </ekz><ekz>
            &ccirc;iuj iliaj verkoj, vortoj kaj agoj &ccirc;iam spiras 
tute klare tiun
            <tld/>an ideon<fnt>Z</fnt>;
          </ekz><ekz>
            ili senkonscie revas pri tio <tld/>a, pri kio ni 
revas<fnt>Z</fnt>;
          </ekz><ekz>
            ili staras nun sur tiu <tld/>a punkto, sur kiu ili staris 
anta&ubreve; 14
            jaroj<fnt>Z</fnt>;
          </ekz><ekz>
            la <ind><tld/>o</ind> okazis al ni, kiel al vi;
            <trd lng="hu">ugyanaz <klr> (a dolog)</klr>&gt;</trd>
            <trd lng="ru">&c_t;&c_o; &c_zh;&c_je; 
&c_s;&c_a;&c_m;&c_o;&c_je;</trd>
          </ekz><ekz>
            la <tld/>o estas pri la alia demando.
          </ekz>
        </dif>
        <trdgrp lng="be">
          <trd>&c_t;&c_o;&c_j; &c_zh;&c_a;</trd>,
          <trd>&c_t;&c_o;&c_j; &c_zh;&c_a; &c_s;&c_a;&c_m;&c_y;</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_t;&c_o;&c_t; &c_zh;&c_je;</trd>,
          <trd>&c_t;&c_o;&c_t; &c_zh;&c_je; &c_s;&c_a;&c_m;&c_y;&c_j;</trd>
        </trdgrp>
      </snc>
