  <trd lng="my">&#x1010;&#x1030;&#x100A;&#x102E;&#x1010;&#x1032;&#x1037;</trd>
  <trd lng="ne">&#x924;&#x94D;&#x92F;&#x938;&#x948;</trd>
  <trdgrp lng="nl">
        <trd>dezelfde</trd>,
        <trd>hetzelfde</trd>,
        <trd>gelijk</trd>
      </trdgrp>
  <trd lng="ny">yemweyo</trd>
  <trd lng="pa">&#xA09;&#xA38;&#xA47; &#xA39;&#xA40;</trd>
  <trd lng="ps">&#x648;&#x631;&#x62A;&#x647;</trd>
  <trdgrp lng="pt">
   <trd>mesmo</trd>
  </trdgrp>
  <trd lng="sd">&#x633;&#x627;&#x6B3;&#x626;&#x64A;</trd>
  <trd lng="si">&#xD91;&#xDB8;</trd>
  <trd lng="sk">ten ist&yacute;</trd>
  <trd lng="sq">i nj&#xEB;jt&#xEB;</trd>
  <trd lng="ta">&#xB85;&#xBA4;&#xBC7;</trd>
  <trd lng="te">&#xC05;&#xC26;&#xC47;</trd>
  <trd lng="th">&#xE40;&#xE14;&#xE35;&#xE22;&#xE27;&#xE01;&#xE31;&#xE19;</trd>
  <trd lng="tl">pareho</trd>
