    <drv mrk="sam.mal0a">
      <kap>mal<tld/>a</kap>
      <snc>
        <dif>
          Diferenca:
          <ekz>
            kompreni&gcirc;ado inter anoj de la plej mal<tld/>aj 
popoloj<fnt>Z</fnt>;
          </ekz><ekz>
            tiu propono estas tute mal<tld/>a ol la mia
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="ali.0a">alia</ref>,
          <ref cel="divers.0a">diversa</ref>,
          <ref cel="vari">varia</ref>.
        </refgrp>
        <refgrp tip="vid">
          <ref cel="mar.0o">maro</ref>,
          <ref cel="fend.0o">fendo</ref>,
          <ref cel="abism.0o">abismo</ref>.
        </refgrp>
      </snc>
  <trdgrp lng="be">
        <trd>&c_ib;&c_n;&c_sh;&c_y;</trd>,
        <trd>&c_a;&c_d;&c_r;&c_o;&c_z;&c_n;&c_y;</trd>
      </trdgrp>
  <trd lng="cs">rozd&iacute;ln&yacute;</trd>
  <trdgrp lng="de">
        <trd>ungleich</trd>,
        <trd>nicht dasselbe</trd>
      </trdgrp>
  <trd lng="en">different</trd>
  <trd lng="es">distinto</trd>
  <trd lng="fr">diff&eacute;rent</trd>
  <trdgrp lng="hu">
        <trd>teljesen m&aacute;s</trd>
        <trd>k&uuml;l&ouml;nb&ouml;z&odblac;</trd>,
        <trd>elt&eacute;r&odblac;</trd>
      </trdgrp>
  <trdgrp lng="it">
        <trd>diverso</trd>,
        <trd>differente</trd>,
        <trd>distinto</trd>
      </trdgrp>
  <trdgrp lng="nl">
        <trd>ongelijk</trd>,
        <trd>verschillend</trd>
      </trdgrp>
  <trdgrp lng="ru">
