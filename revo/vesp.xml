<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vesp</rad>/o
</kap>
<drv mrk="vesp.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vesp.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Iu el familio <ref tip="malprt" cel="vesped.0oj"><tld/>edoj</ref>
      kaj similaj insektoj kun nigra flava desegno kaj venena pikilo:
      <ekz>
        anstata&ubreve; refre&scirc;iga dol&ccirc;o li trovos neston de <tld/>oj
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 14a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Wespe</trd>
    <trd lng="en">wasp</trd>
    <trd lng="hu">dar&aacute;zs</trd>
    <trd lng="nl">wesp</trd>
    <trd lng="sv">geting</trd>
  </snc>
  <snc mrk="vesp.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="vesped.0oj"><tld/>edoj</ref>
      (<trd lng="la">Vespa</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_a;</trd>
  <trd lng="el">&sigma;&phi;&eta_ton;&kappa;&alfa;</trd>
  <trd lng="fr">gu&ecirc;pe</trd>
  <trd lng="pt">vespa</trd>
  <trd lng="ru">&c_o;&c_s;&c_a;</trd>
  <trd lng="sk">osa</trd>
</drv>
</art>
