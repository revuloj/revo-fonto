<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>vesp</rad>/o
</kap>
<drv mrk="vesp.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vesp.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Iu el familio <ref tip="malprt" cel="vesp.0edoj"><tld/>edoj</ref>
      kaj similaj insektoj kun nigra flava desegno kja venena pikilo.
    </dif>
    <trd lng="de">Wespe</trd>
    <trd lng="en">wasp</trd>
    <trd lng="nl">wesp</trd>
  </snc>
  <snc mrk="vesp.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="vesp.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Vespa</trd>).
    </dif>
  </snc>
</drv>
<drv mrk="vesp.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="himeno.0oj">himenopteroj</ref>
      (<trd lng="la">Vespidae</trd>), kiuj faldas la flugilojn dum ripozo.
    </dif>
  </snc>
  <trd lng="de">Faltenwespen</trd>
</drv>
</art>
