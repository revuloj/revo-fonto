<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dias.xml,v 1.15 2023/11/04 14:29:01 revo Exp $">
<kap>
  <rad>dias</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="dias.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="dias.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      <ref tip="dif" cel="permi.0o">Permio</ref>:
      <ekz>
        oni povas imagi ke la tavolo de la <tld/>a (permia) anhidrito, kiu ekz. formas
        tavol&scirc;tupon proksime de <nom>Osterode-D&uuml;na</nom>, deklivas suden
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=507278">Rhume-fonto</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Perm</trd>,
     <trd>Dyas [ark]</trd>
  </trdgrp>
  <trd lng="fr">Permien</trd>
  <trdgrp lng="hu">
     <trd>perm</trd>,
     <trd>di&aacute;sz</trd>
  </trdgrp>
  <trd lng="ja">&#x4E8C;&#x7573;&#x7CFB;  <pr>&#x3075;&#x305F;&#x3058;&#x3087;&#x3046;&#x3051;&#x3044;</pr></trd>
  <trd lng="nl">perm<klr tip="ind"> (tijdperk)</klr></trd>
  <trd lng="pt">terreno <ind>di&aacute;sico</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_i;&c_a;&c_s;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_m;&c_mol;<klr tip="ind"> (&c_g;&c_je;&c_o;&c_l;.)</klr></trd>,
    <trd>&c_p;&c_je;&c_r;&c_m;&c_s;&c_k;&c_i;&c_j; &c_p;&c_je;&c_r;&c_i;&c_o;&c_d;<klr tip="ind">
    (&c_g;&c_je;&c_o;&c_l;.)</klr></trd>
  </trdgrp>
  <trd lng="zh">&#x4E8C;&#x8FED;&#x7D00; <pr>&#xE8;rdi&#xE9;j&#xEC;</pr></trd>
</drv>
</art>

