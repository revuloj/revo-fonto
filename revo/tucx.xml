<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tucx.xml,v 1.20 2024/05/10 10:43:47 revo Exp $">
<kap>
  <rad>tu&ccirc;</rad>/o

</kap>
<drv mrk="tucx.0o">
  <kap><tld/>o
    <fnt><bib>PIV1</bib></fnt>
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="ink.0o">Inko</ref> farita el fumnigro:
      <ekz>
        mi ekhavis la ideon montri al la junulino <klr>[&#x2026;]</klr> etan desegnon faritan
        per <tld/>o, kiu hazarde ku&scirc;is en mia biletujo
        <fnt>
          <bib>Kiso</bib>
          <lok>La Tu&ccirc;desegno</lok>
        </fnt>;
      </ekz>
      <ekz>
        li e&ccirc; <klr>[vidis]</klr> &scirc;iajn brovojn, kiuj formis du delikatajn, kvaza&ubreve; per <tld/>o
        desegnitajn arkojn super &scirc;iaj orkoloraj okuloj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Flugo al la Ebura Turo</lok>
        </fnt>;
      </ekz>
      <ekz>
         la skribo de &ccirc;inaj signoj komence okazis per <tld/>o sur lignotabuloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19681">&Ccirc;ina skribo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="ink.cxina0o">&ccirc;ina inko</ref>
    <ref tip="vid" cel="bistr.0o">bistro</ref>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_sh; <klr tip="amb">(&c_f;&c_a;&c_r;&c_b;&c_a;)</klr></trd>
  <trd lng="de">Tusche</trd>
  <trdgrp lng="en">
    <trd>carbon ink</trd>,
    <trd>Chinese ink</trd>
  </trdgrp>
  <trd lng="hu">tus(tinta)</trd>  
  <trdgrp lng="ja">
    <trd>&#x58A8;  <pr>&#x307C;&#x304F;</pr></trd>,
    <trd>&#x58A8;&#x6C41;  <pr>&#x307C;&#x304F;&#x3058;&#x3085;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">Oostindische inkt</trd>
  <trd lng="pl">tusz</trd>
  <trd lng="pt">nanquim <klr>(tinta)</klr></trd>
  <trd lng="ru">&c_t;&c_u;&c_sh;&c_mol;</trd>
  <trd lng="sv">tusch</trd>
  <trd lng="uk">&c_t;&c_u;&c_sh;</trd>
  <trdgrp lng="zh">
    <trd>&#x58A8;&#x6C41; <pr>m&#xF2;zh&#x12B;</pr></trd>,
    <trd>&#x58A8;&#x6C34; <pr>m&#xF2;shu&#x1D0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="tucx.0i">
  <kap><tld/>i</kap>
  <snc>
    <dif>
      <ref tip="super" cel="desegn.0i.KOMUNE">Desegni</ref> a&ubreve; skribi per <tld/>o:
      <ekz>
        nokte nigra, nigra kest' <tld/>ita
        <fnt>
          <aut>Jozefo E. Nagy</aut>
          <vrk><url ref="http://www.elerno.cn/penseo/PEN244.pdf">Japaneske</url></vrk>
          <lok>Penseo N-ro 244, Feb. 2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">mit <ind>Tusche</ind> schreiben</trd>
    <trdgrp lng="hu">
      <trd>tussal &iacute;r</trd>,
      <trd>tussal rajzol</trd>
    </trdgrp>
    <trd lng="nl">met <ind>Oostindische inkt</ind> schrijven</trd>
    <trdgrp lng="pl">
      <trd>rysowa&cacute; <ind>tuszem</ind></trd>,
      <trd>kre&sacute;li&cacute; w <ind>tuszu</ind></trd>
    </trdgrp>
    <trd lng="ru">&c_t;&c_u;&c_sh;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
    <trd lng="sv">rita eller skriva med <ind>tusch</ind></trd>
  </snc>
  <snc>
    <ref tip="dif" cel="lav.0umi">lavumi</ref>
    <ref tip="vid" cel="tusx.re0i">retu&scirc;i</ref>
    <trd lng="de">tuschen</trd>
    <trdgrp lng="hu">
      <trd>elmos <klr tip="ind">(&aacute;rnyal)</klr></trd>,
      <trd>lav&iacute;roz <klr tip="ind">(&aacute;rnyal)</klr></trd>,
      <trd>&aacute;rnyal</trd>
    </trdgrp>
    <trd lng="ru">&c_z;&c_a;&c_t;&c_u;&c_sh;&c_jo;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_u;&c_sh;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol; <ind>&c_t;&c_u;&c_sh;&c_sh;&c_u;</ind></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x58A8;&#x3067;&#x304B;&#x304F;  <pr>&#x3059;&#x307F;&#x3067;&#x304B;&#x304F;</pr></trd>,
    <trd>&#x8584;&#x304F;&#x5857;&#x308B;  <pr>&#x3046;&#x3059;&#x304F;&#x306C;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_m;&c_a;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i; &c_t;&c_u;&c_sh;&c_sh;&c_ju;</trd>,
    <trd>&c_t;&c_u;&c_sh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_u;&c_sh;&c_o;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6CFC;&#x58A8;&#x4F5C;&#x753B; <pr>p&#x14D;m&#xF2;zu&#xF2;hu&#xE0;</pr></trd>,
    <trd>&#x6F51;&#x58A8;&#x4F5C;&#x756B; <pr>p&#x14D;m&#xF2;zu&#xF2;hu&#xE0;</pr></trd>
  </trdgrp>
</drv>
</art>
