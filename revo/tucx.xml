<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tucx.xml,v 1.14 2020/11/19 01:11:13 revo Exp $">
<kap>
  <rad>tu&ccirc;</rad>/o

</kap>
<drv mrk="tucx.0o">
  <kap><tld/>o
    <fnt><bib>PIV1</bib></fnt>
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="ink.0o">Inko</ref> farita el fumnigro:
      <ekz>
        li e&ccirc; <klr>[vidis]</klr> &scirc;iajn brovojn, kiuj formis du delikatajn, kvaza&ubreve; per <tld/>o
        desegnitajn arkojn super &scirc;iaj orkoloraj okuloj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Flugo al la Ebura Turo</lok>
        </fnt>;
      </ekz>
      <ekz>
         la skribo de &ccirc;inaj signoj komence okazis per <tld/>o sur lignotabuloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19681">&Ccirc;ina skribo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="ink.cxina0o">&ccirc;ina inko</ref>
    <ref tip="vid" cel="bistr.0o">bistro</ref>
  </snc>
  <trd lng="de">Tusche</trd>
  <trdgrp lng="en">
    <trd>carbon ink</trd>,
    <trd>Chinese ink</trd>
  </trdgrp>
  <trd lng="hu">tus(tinta)</trd>  
  <trd lng="nl">Oostindische inkt</trd>
  <trd lng="pl">tusz</trd>
  <trd lng="pt">nanquim <klr>(tinta)</klr></trd>
  <trd lng="ru">&c_t;&c_u;&c_sh;&c_mol;</trd>
  <trd lng="sv">tusch</trd>
</drv>

<drv mrk="tucx.0i">
  <kap><tld/>i</kap>
  <snc>
    <dif>
      Desegni a&ubreve; skribi per <tld/>o.
    </dif>
    <trd lng="de">mit Tusche schreiben</trd>
    <trdgrp lng="hu">
      <trd>tussal &iacute;r</trd>,
      <trd>tussal rajzol</trd>
    </trdgrp>
    <trd lng="nl">met <ind>Oostindische inkt</ind> schrijven</trd>
    <trdgrp lng="pl">
      <trd>rysowa&cacute; <ind>tuszem</ind></trd>,
      <trd>kre&sacute;li&cacute; w <ind>tuszu</ind></trd>
    </trdgrp>
    <trd lng="ru">&c_t;&c_u;&c_sh;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
    <trd lng="sv">rita eller skriva med <ind>tusch</ind></trd>
  </snc>
  <snc>
    <dif>
      <ref tip="dif" cel="lav.0umi">Lavumi</ref>.
    </dif>
    <ref tip="vid" cel="tusx.re0i">retu&scirc;i</ref>
    <trdgrp lng="hu">
      <trd>elmos <klr tip="ind">(&aacute;rnyal)</klr></trd>,
      <trd>lav&iacute;roz <klr tip="ind">(&aacute;rnyal)</klr></trd>,
      <trd>&aacute;rnyal</trd>
    </trdgrp>
    <trd
    lng="ru">&c_z;&c_a;&c_t;&c_u;&c_sh;&c_jo;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
</drv>
</art>
