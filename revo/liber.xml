<drv mrk="liber.mal0igi">
  <kap>mal<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="liber.mal0igi.KOMUNE">
    <dif>
      Senigi je la&ubreve;vola irado, agado:
      <ekz>
        oni do lin arestis, liajn skribojn forbruligis, lin mem
        mal<tld/>igis
        <fnt><bib>SatirRak</bib>, <lok>la falsa a&ubreve;guro</lok></fnt>.
      </ekz>
      <ekz>
        oni ne povas mal<tld/>igi homojn pro tio, ke ili volis konvinki&gcirc;i, &ccirc;u la
        sankta Nilo levi&gcirc;as, a&ubreve; pro tio, ke ili a&ubreve;skultis muzikon
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        en tiaj okazoj, kie kelka <tld/>eco alportas al ni nenian malutilon, kial ni devus senbezone
        nin mal<tld/>igi?
        <fnt>
          <bib>LR</bib>
          <vrk>Respondo 38, La Revuo, 1908, Majo</vrk>
          <lok>1. Vortfarado</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">empresonar</trd>
  <trdgrp lng="cs">
    <trd>uv&ecaron;znit</trd>,
    <trd>zatknout</trd>,
    <trd>zbavit svobody</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gefangen nehmen</trd>,
    <trd>festsetzen</trd>
  </trdgrp>
  <trd lng="en">imprison</trd>
  <trdgrp lng="es">
    <trd>encarcelar</trd>,
    <trd>recluir</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>emprisonner</trd>,
    <trd>incarc&eacute;rer</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>imprigionare</trd>,
    <trd>incarcerare</trd>
    <trd>rinchiudere<klr> (in carcere)</klr></trd>
  </trdgrp>
  <trd lng="pl">wi&eogonek;zi&cacute;</trd>     
  <trd lng="ro">re&#x21B;ine</trd>
  <trdgrp lng="sk">
    <trd>uv&auml;zni&tcaron;</trd>,
    <trd>zatkn&uacute;&tcaron;</trd>
  </trdgrp>
</drv>

