<drv mrk="liber.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="liber.0igi.KOMUNE">
    <dif>
      Meti en <tld/>an staton:
      <ekz>
        <tld/>igi malliberulon, sklavon;
      </ekz>
      <ekz>
        li retenis &scirc;in per la brako <klr>[sed]</klr> &scirc;i <tld/>igis
        sin
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;ipanaro kun estimego <tld/>igis al li vojon
        <fnt><bib>FK</bib>, <lok>4, rakontoj, nokto, (de Eube el
        Odeso)</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>igi la vojon de &scirc;tonoj
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 62:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igi iun el sklaveco
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 6:6</lok>
        </fnt>
        <fnt>
          <bib>MT</bib>
          <lok>Readmono</lok>
        </fnt>,
      </ekz>
      <ekz>
        de timo, de lia promeso, de mensoga akuzo, de siaj &scirc;uldoj;
      </ekz>
      <ekz>
        <tld/>igita de sia respondeco
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igita de duboj;
      </ekz>
      <ekz>
        klera homo <tld/>igita de &ccirc;ia anta&ubreve;ju&gcirc;o;
      </ekz>
      <ekz>
        <tld/>igi la lingvon de senutila balasto;
      </ekz>
      <ekz>
        ellerninte unu <klr>[sufikson]</klr> ni estas <tld/>igitaj de la lernado de
        granda serio da vortoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiu diplomo <tld/>igas de militista servado;
      </ekz>
      <ekz>
        sin <tld/>igi de <klr>(a&ubreve; el sub)</klr> fremda jugo;
      </ekz>
      <ekz>
        la naci<tld/>iga movado disvolvi&gcirc;as.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="emanci.0i">emancipi</ref>,
