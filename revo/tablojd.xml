<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tablojd.xml,v 1.17 2023/10/21 13:02:26 revo Exp $">
<kap>
  <rad>tablojd</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="tablojd.0o">
  <kap><tld/>o</kap>
  <snc mrk="tablojd.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="sin" cel="prem.0ajxo.MED">Prema&jcirc;o</ref> por
      enbu&scirc;igo, ordinare pli granda ol
      <ref tip="vid" cel="pilol.0o.MED">pilolo</ref>, kiun oni pli
      facile prenas solvante &gcirc;in en akvo a&ubreve; lasante
      solvi&gcirc;i en la bu&scirc;o:
      <ekz>
        englutinte unu <tld/>on post &ccirc;irka&ubreve; 15 minutoj
        &scirc;i bonfartis
        <fnt>
        <aut>Henri Masson</aut>,
        <vrk>Medikamento kontra&ubreve; migreno</vrk>,
        <lok><url ref="http://www.eventoj.hu/arkivo/eve-146.htm">
        Eventoj, numero 146a, 1998-04</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        c-vitaminaj <tld/>oj
        <fnt><bib>Viki</bib>,
        <lok>artikolo &leftquot;skorbuto&rightquot;, 2005-10-21</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kapsul.0o.MED">kapsulo</ref>,
      <ref cel="oblat.0o.MED">oblato</ref>,
      <ref cel="pastel.0o.MED">pastelo</ref>,
      <ref cel="pilol.0o.MED">pilolo</ref>,
      <ref cel="prem.0ajxo.MED">prema&jcirc;o</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_b;&c_l;&c_je;&c_t;&c_k;&c_a;</trd>
  <trd lng="ca">comprimit</trd>
  <trd lng="cs">tableta</trd>
  <trd lng="de">Tablette</trd>
  <trdgrp lng="en">
    <trd>tabloid<klr tip="amb"> (pill)</klr></trd>,
    <trd>tablet<klr tip="amb"> (pill)</klr></trd>
  </trdgrp>
  <trd lng="fr">comprim&eacute;<klr> (pharm.)</klr></trd>
  <trd lng="ja">&#x9320;&#x5264;  <pr>&#x3058;&#x3087;&#x3046;&#x3056;&#x3044;</pr></trd>
  <trdgrp lng="pl">
    <trd>tabletka</trd>,
    <trd>pastylka</trd>,
    <trd>lingwetka</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>tabl&oacute;ide</trd>,
   <trd>comprimido</trd>,
   <trd>pastilha</trd>
   </trdgrp>
  <trd lng="ro">comprimat</trd>
  <trd lng="sk">tabletka</trd>
  <trd lng="uk">&c_t;&c_a;&c_b;&c_l;&c_je;&c_t;&c_k;&c_a;</trd>
</drv>
</art>

