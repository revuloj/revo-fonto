<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kin1.xml,v 1.7 2022/03/21 17:51:58 revo Exp $">
<kap>
  <rad>kin</rad>/o
  <fnt>V</fnt>
</kap>
<drv mrk="kin1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="kinkon.0o.KEM">Kinkono<sncref/></ref>:
      <ekz>
        la nomo &leftquot;<tld/>o&rightquot; venas de la ke&ccirc;ua vorto &leftquot;<nac>kina</nac>&rightquot; (&leftquot;&scirc;elo&rightquot;).
      </ekz>
    </dif>
    <refgrp tip="hom">
      Ne miksu kun <ref cel="kin.0o">kino</ref> (arto).
    </refgrp>
  </snc>
  <trd lng="cs">kina</trd>
  <trdgrp lng="de">
    <trd>Chinarinde</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kinabast<klr tip="ind"> (arch.)</klr></trd>,
    <trd>kinaboom<klr tip="ind"> (arch.)</klr></trd>
  </trdgrp>
  <trd lng="pl">chinowiec</trd>     
  <trd lng="ro">chinchin&abreve;</trd>
  <trd lng="sk">kina</trd>
  <trd lng="uk">&#x43A;&#x456;&#x43D;&#x43E;</trd>
</drv>
</art>

