<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: koliri.xml,v 1.22 2023/12/03 09:58:52 revo Exp $">
<kap>
  <rad>koliri</rad>/o
  <fnt><bib>NT</bib></fnt>
</kap>

<drv mrk="koliri.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Flua&jcirc;a <ref tip="super" cel="medika1.0o">medikamento</ref>, aplikata sur la malsanan 
      <ref cel="konjun4.0o">konjunktivon</ref>:
      <ekz>
        mi konsilas al vi a&ccirc;eti de mi <klr>[&#x2026;]</klr> <tld/>on, por &scirc;miri viajn okulojn, por ke vi vidu 
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 3:18</lok>
        </fnt>;
      </ekz>
       <ekz>
        <tld/>o (okul-akvo, okulgutoj)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=284569">Listo de mallongigoj sur receptoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_m;&c_je;&c_h;&c_l;&c_je;&c_m; &c_z;&c_a; &c_o;&c_ch;&c_i;<klr> (se &scirc;mira&jcirc;o)</klr></trd>,
    <trd>&c_k;&c_a;&c_p;&c_k;&c_i; &c_z;&c_a; &c_o;&c_ch;&c_i;<klr> (se gutoj)</klr></trd>
  </trdgrp>
  <trd lng="ca">col&middot;liri</trd>
  <trd lng="cs">kolyria</trd>
  <trd lng="de">Augentropfen</trd>
  <trdgrp lng="en">
    <trd>eye salve<klr> (se &scirc;mira&jcirc;o)</klr></trd>,
    <trd>eye wash<klr> (se likva kaj per taseto)</klr></trd>,
    <trd>eye drops<klr> (se gutoj)</klr></trd>
  </trdgrp>
  <trd lng="es">colirio</trd>
  <trd lng="fr">collyre</trd>
  <trd lng="ja">&#x76EE;&#x85AC;  <pr>&#x3081;&#x3050;&#x3059;&#x308A;</pr></trd>
  <trdgrp lng="nl">
    <trd>oogzalf<klr> (se &scirc;mira&jcirc;o)</klr></trd>,
    <trd>oogwater<klr> (se likva)</klr></trd>,
    <trd>oogdruppels<klr> (se gutoj)</klr></trd>
  </trdgrp>
  <trd lng="pl">krople do oczu</trd>
  <trd lng="pt">col&iacute;rio</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_l;&c_a;&c_z;&c_n;&c_y;&c_je; &c_k;&c_a;&c_p;&c_l;&c_i; <klr>(se gutoj)</klr></trd>,
    <trd>&c_n;&c_a;&c_r;&c_u;&c_zh;&c_n;&c_o;&c_je; &c_g;&c_l;&c_a;&c_z;&c_n;&c_o;&c_je; &c_s;&c_r;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">o&ccaron;n&eacute; kvapky</trd>
  <trd lng="uk">&#x43A;&#x440;&#x430;&#x43F;&#x43B;&#x456; &#x434;&#x43B;&#x44F; &#x43E;&#x447;&#x435;&#x439;</trd>
  <trdgrp lng="zh">
    <trd>&#x773C;&#x836F; <pr>y&#x1CE;ny&#xE0;o</pr></trd>,
    <trd>&#x773C;&#x836F;&#x6C34; <pr>y&#x1CE;ny&#xE0;oshu&#x1D0;</pr></trd>
  </trdgrp>
</drv>
</art>
