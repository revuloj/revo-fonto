<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>zingibr</rad>/o
</kap>
<drv mrk="zingib.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="zingibr.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Zingiber</trd>),
      kun tubereca olehava rizomo,
      kiu estas uzata kiel spica&jcirc;o.
    </dif>
  </snc>
  <trd lng="de">Ingwer</trd>
  <trd lng="es">jengibre</trd>
  <trd lng="fr">gingembre</trd>
  <trd lng="nl">gember</trd>
  <trd lng="ru">&c_i;&c_m;&c_b;&c_i;&c_r;&c_mol;</trd>
</drv>
<drv mrk="zingibr.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de tropikaj vegeta&jcirc;oj (<trd lng="la">Zingiberaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Ingwergew&auml;chse</trd>
</drv>
</art>
