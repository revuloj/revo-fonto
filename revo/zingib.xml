<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>zingibr</rad>/o
</kap>
<drv mrk="zingib.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="zingib1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Zingiber</trd>), kun tubereca olehava rizomo,
      kiu estas uzata kiel spica&jcirc;o.
    </dif>
  </snc>
  <trd lng="de">Ingwer</trd>
  <trd lng="el">&pi;&jota;&pi;&epsilon;&rho;&omikron_ton;&rho;&rho;&jota;&zeta;&alfa;</trd>
  <trd lng="en">ginger</trd>
  <trd lng="es">jengibre</trd>
  <trd lng="fr">gingembre</trd>
  <trd lng="hu">gy&ouml;mb&eacute;r</trd>
  <trdgrp lng="it">
    <trd>ginepro</trd>,
    <trd>zenzebro</trd>
  </trdgrp>
  <trd lng="nl">gember</trd>
  <trd lng="pt">gengibre</trd>
  <trd lng="ru">&c_i;&c_m;&c_b;&c_i;&c_r;&c_mol;</trd>
  <trd lng="sv">ingef&auml;ra</trd>
</drv>

</art>
