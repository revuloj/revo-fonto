<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>bakteriofag</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="bakteriofag.0o">
  <kap><tld/>o</kap>
  <snc mrk="bakteriofag.0o.BAK">
    <uzo tip="fak">BAK</uzo>
    <dif>
      <ref tip="super" cel="virus.0o.BAK">Viruso</ref>,
      kiu infektas bakteriojn:
      <ekz>
        <tld/>oj <klr>...</klr> estas eta&jcirc;oj simil<klr>aj</klr> al
        virusoj
        kiuj atakas la &ccirc;elojn de la bakterioj
        <fnt>
          <aut>[A. Oberndorfer]</aut>,
          <vrk><url
          ref="http://esperantaretradio.blogspot.fr/2013/06/nia-paralela-imunsistemo.html">
          Nia paralela imunsistemo</url></vrk>,
          <lok>Esperanta Retradio, 2013-06-21</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>bacteriophage</trd>,
    <trd>phage</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bact&eacute;riophage</trd>,
    <trd>phage</trd>
  </trdgrp>
  <trd lng="id">bakteriofag</trd>
</drv>
</art>

