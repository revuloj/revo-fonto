<drv mrk="bend.glu0o">
  <kap>glu<tld/>o</kap> <fnt><bib>EBV</bib></fnt>
  <snc mrk="bend.glu0o.KOMUNE">
    <dif>
      Maldika <tld/>o kun unu glua flanko, uzata por fermo de
      paka&jcirc;o, &scirc;topo de fendoj...:
      <ekz>
        nek magneta fiksilo, nek glu<tld/>o efikas
        <fnt>
          <bib>ChR</bib>
          <lok>Spiona rondo</lok>
        </fnt>
      </ekz>
      <ekz>
        la centra po&scirc;tejo ne akceptas ordinaran glu<tld/>on
        <fnt>
          <aut>Filippo</aut>,
          <vrk><url ref="&Monato;/2006/006950.php">Moderna po&scirc;to</url></vrk>,
          <bib>Monato</bib>,
          <lok>2006/04, p. 18</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="diakil.0o">diakilo</ref>
    <trdgrp lng="zh">
    <trd>&#x5355;&#x9762;&#x80F6; <pr>d&#x101;nmi&#xE0;nji&#x101;o</pr></trd>,
    <trd>&#x80F6;&#x7C98;&#x5E26; <pr>ji&#x101;oni&#xE1;nd&#xE0;i</pr></trd>,
    <trd>&#x7D55;&#x7DE3;&#x81A0;&#x5E03; <pr>ju&#xE9;yu&#xE1;nji&#x101;ob&#xF9;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_k;&c_l;&c_je;&c_j;&c_k;&c_a;&c_ja; <ind>&c_s;&c_t;&c_u;&c_zh;&c_k;&c_a;</ind></trd>
