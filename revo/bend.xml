      <ekz>
        mi ofte a&ubreve;dis homojn diri
        &leftquot;sur<tld/>igilo&rightquot; anstata&ubreve; la
        oficiala
        &leftquot;magnetofono&rightquot;
        <fnt><bib>BonaLingvo</bib>, <lok>p. 69a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_g;&c_n;&c_ib;&c_t;&c_a;&c_f;&c_o;&c_n;</trd>
  <trd lng="en">tape recorder</trd>
