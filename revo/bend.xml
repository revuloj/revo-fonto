  </trdgrp>
  <trdgrp lng="es">
    <trd>cinta adhesiva</trd>,
    <trd>papel celo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bande adh&eacute;sive</trd>,
    <trd>scotch</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7C98;&#x7740;&#x30C6;&#x30FC;&#x30D7;  <pr>&#x306D;&#x3093;&#x3061;&#x3083;&#x304F;&#x30C6;&#x30FC;&#x30D7;</pr></trd>,
    <trd>&#x30BB;&#x30ED;&#x30C6;&#x30FC;&#x30D7;</trd>
