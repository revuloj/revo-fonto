<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: instit1.xml,v 1.25 2026/02/10 19:22:41 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>institut</rad>/o
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="instit1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="instit1.0o.scienca">
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="super" cel="instit.0o">Institucio</ref> por scienco kaj esplorado (altlernejoj,
      a&ubreve; korporacioj sciencaj kaj artistaj):
      <ekz>
	<tld/>o de &hcirc;emio, de in&gcirc;enierarto, de komerco;
      </ekz>
      <ekz>
        oni malfermis <klr>[&#x2026;]</klr> instituton de orientaj lingvoj
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiun superan lernejon vi estas fininta? &ndash; la petrogradan Politeknikan <tld
        lit="I"/>on
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        Japana Esperanto-<tld lit="I"/>o
        <fnt>
          <bib>EeP</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld lit="I"/>o por Altaj Ekonomiaj kaj Financaj Studoj
        <fnt>
          <bib>EeP</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Norvega Nobel-<tld lit="I"/>o
        <fnt>
          <bib>EeP</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Geografia <tld lit="I"/>o de la Akademio de Scienco de Sovetunio
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_ib;&c_n;&c_s;&c_t;&c_y;&c_t;&c_u;&c_t; <klr>(&c_n;&c_a;&c_v;&c_u;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_y; &c_a;&c_b;&c_o; &c_n;&c_a;&c_v;&c_u;&c_k;&c_o;&c_v;&c_y;)</klr></trd>
    <trd lng="de"><klr>(wissenschaftliches)</klr> Institut</trd>
    <trd lng="en">institute</trd>
    <trd lng="es">instituto</trd>
    <trd lng="fr">institut<klr tip="ind">
    (scientifique ou artistique)</klr></trd>
    <trd lng="hu">int&eacute;zet</trd>
    <trdgrp lng="ja">
      <trd>&#x7814;&#x7A76;&#x6240;  <pr>&#x3051;&#x3093;&#x304D;&#x3085;&#x3046;&#x3058;&#x3087;</pr></trd>,
      <trd>&#x5B66;&#x9662;  <pr>&#x304C;&#x304F;&#x3044;&#x3093;</pr></trd>,
      <trd>&#x5B66;&#x4F1A;  <pr>&#x304C;&#x3063;&#x304B;&#x3044;</pr></trd>
    </trdgrp>
    <trd lng="nl">instituut</trd>
    <trd lng="pl">instytut</trd>
    <trd lng="pt">instituto</trd>
    <trd lng="ru">&c_i;&c_n;&c_s;&c_t;&c_i;&c_t;&c_u;&c_t;
    <klr>(&c_n;&c_a;&c_u;&c_ch;&c_n;&c_y;&c_j;,
    &c_u;&c_ch;&c_je;&c_b;&c_n;&c_y;&c_j; &c_i; &c_t;.&c_p;.)</klr></trd>
    <trdgrp lng="sk">
      <trd>in&scaron;tit&uacute;t</trd>,
      <trd>&uacute;stav</trd>
    </trdgrp>
    <trdgrp lng="cs">
      <trd>institut</trd>,
      <trd>&uacute;stav</trd>
    </trdgrp>
    <trd lng="uk">&c_ib;&c_n;&c_s;&c_t;&c_i;&c_t;&c_u;&c_t;</trd>
    <trdgrp lng="zh">
      <trd>&#x5B66;&#x9662; <pr>xu&eacute;yu&agrave;n</pr></trd>,
      <trd>&#x6240; <pr>su&#x1D2;</pr></trd>,
      <trd>&#x827A;&#x672F;&#x5B66;&#x9662; <pr>y&igrave;sh&ugrave;xu&eacute;yu&agrave;n</pr></trd>,
      <trd>&#x5B9E;&#x9A8C;&#x6240; <pr>sh&iacute;y&agrave;nsu&#x1D2;</pr></trd>
    </trdgrp>
  </snc>
  <snc mrk="instit1.0o.edukejo">
    <uzo tip="fak">PED</uzo>
    <dif>
      <ref tip="dif" cel="eduk.0ejo">Edukejo</ref>:
      <ekz>
        <nom>Rika</nom> sekve ne vizitadis la <tld lit="I"/>on de la surdmutuloj kaj restadis hejme
        &ccirc;e siaj gepatroj kaj gefratoj
        <fnt>
          <bib>IdO</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malnova <tld/>o por fra&ubreve;linoj
        <fnt>
          <bib>Fab4</bib>
          <lok>Sonorila profundo</lok>
        </fnt>;
      </ekz>
      <ekz>
        miaj du filoj estas kadetoj en prusa kadeta <tld/>o
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="lern.logx0ejo">lo&gcirc;lernejo</ref>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_v;&c_u;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; <ind>&c_w;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_a;</ind></trd>,
      <trd>&c_v;&c_u;&c_ch;&c_e;&c_l;&c_mol;&c_n;&c_ja;</trd>
    </trdgrp>
    <trd lng="de">Erziehungsanstalt</trd>
  </snc>
</drv>
</art>
