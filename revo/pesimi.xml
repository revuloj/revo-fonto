<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>pesimist</rad>/o
</kap>
<drv mrk="pesimi.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Pesimismano.
    </dif>
  </snc>
  <snc>
    <dif>
      Homo, kiu &ccirc;iam vidas nur la malbonan flankon en &ccirc;iuj aferoj.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="vid.nigre0ulo">nigrevidulo</ref>
  </refgrp>
  <trd lng="de">Pessimist</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;faitiste</trd>,
    <trd>pessimiste</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>pessimist</trd>,
    <trd>zwartkijker</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_s;&c_s;&c_i;&c_m;&c_i;&c_s;&c_t;</trd>
</drv>
</art>
