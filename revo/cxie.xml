<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cxie.xml,v 1.35 2023/10/31 13:37:03 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&ccirc;ie</rad> <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="cxie.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc mrk="cxie.0.KOMUNE">
    <gra><vspec>adverbo</vspec></gra>
    <dif>
      En &ccirc;iu loko:
      <ekz>
        li voja&gcirc;as <tld/>n, kaj <tld/> oni lin renkontas;
      </ekz>
      <ekz>
        nenie semata, <tld/> trovata
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_s;&c_ju;&c_d;&c_y; <klr>(&c_d;&c_z;&c_je;)</klr></trd>,
    <trd>&c_p;&c_a;&c_w;&c_s;&c_ju;&c_d;&c_n;&c_a;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_o;&c_z;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_n;&c_a;&c_v;&c_s;&c_ja;&c_k;&c_malmol;&c_d;&c_je;</trd>
  <trd lng="bo">&#xF42;&#xF0B;&#xF66;&#xF0B;&#xF42;&#xF0B;&#xF63;&#xF0B;</trd>
  <trd lng="de">&uuml;berall</trd>
  <trd lng="he">&#x5D1;&#x5DB;&#x5DC; &#x5DE;&#x5E7;&#x5D5;&#x5DD;</trd>
  <trdgrp lng="ja">
    <trd>&#x3044;&#x305F;&#x308B;&#x6240;&#x306B;  <pr>&#x3044;&#x305F;&#x308B;&#x3068;&#x3053;&#x308D;&#x306B;</pr></trd>,
    <trd>&#x3069;&#x3053;&#x3067;&#x3082;</trd>
  </trdgrp>
  <trd lng="nl">overal</trd>
  <trd lng="en">everywhere</trd>
  <trdgrp lng="es">
    <trd>por todas partes</trd>,
    <trd>en todas partes</trd>
  </trdgrp>
  <trd lng="fr">partout</trd>
  <trdgrp lng="hu">
    <trd>mindenhol</trd>,
    <trd>minden&uuml;tt</trd>
  </trdgrp>
  <trd lng="ms">hampir semua</trd>
  <trd lng="pl">wsz&eogonek;dzie</trd>
  <trd lng="pt">em todo o lugar</trd>
  <trdgrp lng="ro">
    <trd>peste tot</trd>,
    <trd>pretutindeni</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_z;&c_d;&c_je;</trd>,
    <trd>&c_v;&c_s;&c_ju;&c_d;&c_u;</trd>
  </trdgrp>
  <trd lng="sk">v&scaron;ade</trd>
  <trd lng="sv">&ouml;verallt</trd>
  <trdgrp lng="sw">
    <trd>kila mahali</trd>,
    <trd>popote</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43A;&c_r;&c_ib;&c_z;&#x44C;</trd>,
    <trd>&c_u;&c_s;&#x44E;&c_d;&c_i;</trd>
  </trdgrp>
  <trd lng="vo">&uuml;bal</trd>
  <trdgrp lng="zh">
    <trd>&#x516B;&#x4E0B;&#x91CC; <pr>b&#x101;xi&#xE0;l&#x1D0;</pr></trd>,
    <trd>&#x4E94;&#x6D32; <pr>w&#x1D4;zh&#x14D;u</pr></trd>,
    <trd>&#x5404;&#x5904; <pr>g&#xE8;ch&#xF9;</pr></trd>,
    <trd>&#x4E16;&#x754C;&#x5404;&#x5904; <pr>sh&#xEC;ji&#xE8;g&#xE8;ch&#xF9;</pr></trd>,
    <trd>&#x4E16;&#x754C;&#x5404;&#x5730; <pr>sh&#xEC;ji&#xE8;g&#xE8;d&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="cxie.0n">
  <kap><tld/>n</kap>
  <snc>
    <dif>
      Al &ccirc;iu loko
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_s;&c_ju;&c_d;&c_y; <klr>(&c_k;&c_u;&c_d;&c_y;)</klr></trd>
  <trd lng="de">&uuml;berallhin</trd>
  <trd lng="he">&#x5DC;&#x5DB;&#x5DC; &#x5DE;&#x5E7;&#x5D5;&#x5DD;</trd>
  <trd lng="ja">&#x3044;&#x305F;&#x308B;&#x6240;&#x3078;  <pr>&#x3044;&#x305F;&#x308B;&#x3068;&#x3053;&#x308D;&#x3078;</pr></trd>
  <trd lng="nl">overal heen</trd>
</drv>

<drv mrk="cxie.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Trovebla, estanta en &ccirc;iu loko:
      <ekz>
        La dioj sendu al li <klr>(al la imperiestro)</klr> longan
        vivon, &mdash; tuj ali&gcirc;is Pilato, &mdash; kaj la <tld/>an pacon
        <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 25a</lok></fnt>;
      </ekz><ekz>
        <uzo tip="fak">MAT</uzo>
        <ref cel="cxie.0afunkcio"><tld/>a funkcio</ref>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_w;&c_s;&c_ju;&c_d;&c_n;&c_y;</trd>
  <trd lng="en">ubiquitous</trd>
  <trd lng="ja">&#x3044;&#x305F;&#x308B;&#x6240;&#x306E;  <pr>&#x3044;&#x305F;&#x308B;&#x3068;&#x3053;&#x308D;&#x306E;</pr></trd>
  <trd lng="ru">&c_p;&c_o;&c_v;&c_s;&c_je;&c_m;&c_je;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="cxie.0afunkcio">
  <kap><tld/>a funkcio</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>KompLeks</bib></fnt>
  <snc>
    <dif>
      <ref cel="funkci.0o.MATEM">Funkcio<sncref/></ref>
      <frm am="f: X -&gt; Y">f: X&#x2192;Y</frm> estas <tld/>a funkcio super
      <frm am="X">X</frm>, se
      &gcirc;i estas difinita (konver&gcirc;as) por &ccirc;iu <frm am="x">x</frm>
      el <frm am="X">X</frm>:
      <ekz>
        problemo: &ccirc;u ekzistas <tld/>a komputebla funkcio, kiu ne estas
        primitive rekursia funkcio
        <fnt><bib>KompLeks</bib>,
           <lok>art. &leftquot;<url ref="http://bertilow.com/div/komputada_leksikono/DI.html#DIAGONALADO">diagonalado</url>&rightquot;</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_r;&c_o;&c_z;&c_mol; &c_v;&c_y;&c_z;&c_n;&c_a;&c_ch;&c_a;&c_n;&c_a;&c_ja; <ind>&c_f;&c_u;&c_n;&c_k;&c_c;&c_y;&c_ja;</ind></trd>
  <trd lng="en">total function</trd>
  <trd lng="ru">&c_v;&c_s;&c_ju;&c_d;&c_u; &c_o;&c_p;&c_r;&c_je;&c_d;&c_je;&c_l;&c_jo;&c_n;&c_n;&c_a;&c_ja; &c_f;&c_u;&c_n;&c_k;&c_c;&c_i;&c_ja;</trd>
</drv>

<drv mrk="cxie.preskaux0">
  <kap>preska&ubreve; <tld/></kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc mrk="cxie.preskaux0.MAT">
    <dif>
      <klr>(rilate iun <ref cel="mezur.0o.MAT">mezuron</ref>)</klr>
      Krom &ccirc;e la punktoj de nulmezura (sub)aro:
      <ekz>
        funkcio kun numerebla <ref cel="ten.sub0anto.MAT">subtenanto
        </ref> estas preska&ubreve; <tld/> nula <klr>(rilate la lebegan
        mezuron)</klr>;
      </ekz><ekz>
        du preska&ubreve; <tld/> egalaj funkcioj havas saman lebegan
        integralon.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_m;&c_a;&c_l;&c_mol; &c_s;&c_k;&c_r;&c_o;&c_z;&c_mol;</trd>,
      <trd>&c_a;&c_m;&c_a;&c_l;&c_mol; &c_u;&c_s;&c_ju;&c_d;&c_y;</trd>
    </trdgrp>
    <trd lng="de">fast &uuml;berall</trd>
    <trd lng="en">almost everywhere</trd>
    <trd lng="fr">presque partout</trd>
    <trd lng="hu">majdnem mindenhol</trd>
    <trd lng="pl">prawie wsz&eogonek;dzie</trd>
    <trd lng="ru">&c_p;&c_o;&c_ch;&c_t;&c_i; &c_v;&c_s;&c_ju;&c_d;&c_u;</trd>
  </snc>
</drv>



</art>
