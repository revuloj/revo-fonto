<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>disput</rad>/i
</kap>
<drv mrk="disput.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Malpaci akre diskutante:
      <ekz>
        kial vi volas <tld/>i kun mi
        <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Jeremia 2, 29</lok>)</fnt>?
      </ekz>
      <ekz>
        ili inter<tld/>is pri la rango;
      </ekz>
      <ekz>
        <tld/>ema virino;
      </ekz>
      <ekz>
        la loto &ccirc;esigas <tld/>ojn
        <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Sentencoj 18, 18</lok>)</fnt>;
      </ekz>
      <ekz>
        pacienculo kvietigas <tld/>on
        <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Sentencoj 15, 18</lok>)</fnt>;
      </ekz>
      <ekz>
        pasanto, kiu sin miksas en malpropran <tld/>on
        <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Sentencoj 26, 17</lok>)</fnt>;
      </ekz>
      <ekz>
        havi ne<tld/>eblan a&ubreve;toritatecon<fnt>Z</fnt>.
      </ekz>  
    </dif> 
    <refgrp tip="sin">
      <ref cel="kverel.0i">kvereli</ref>,
      <ref cel="polemi.0i">polemiki</ref>,
      <ref cel="konfli.0i">konflikti</ref>,
      <ref cel="kontes.0i">kontesti</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">p&rcaron;&iacute;t se</trd>
  <trd lng="de">streiten</trd>
  <trd lng="en">dispute</trd>
</drv>
<drv mrk="disput.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Akre diskuti pri io.  
    </dif>
  </snc>
</drv>
</art>
<!--
$Log$
-->
</vortaro>