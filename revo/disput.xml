<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>disput</rad>/i
</kap>
<drv mrk="disput.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Malpaci akre diskutante:
      <ekz>
        kial vi volas <tld/>i kun mi
        <fnt><bib>MT</bib>, <lok>&Jer; 2:29</lok></fnt>?
      </ekz>
      <ekz>
        ili inter<tld/>is pri la rango;
      </ekz>
      <ekz>
        <ind><tld/>ema</ind> virino;
        <trd lng="fr">querelleur<klr tip="ind"> (adj.)</klr></trd>
      </ekz>
      <ekz>
        la loto &ccirc;esigas <ind><tld/>o</ind>jn
        <fnt><bib>MT</bib>, <lok>&Sen; 18:18</lok></fnt>;
        <trd lng="fr">dispute</trd>
      </ekz>
      <ekz>
        pacienculo kvietigas <tld/>on
        <fnt><bib>MT</bib>, <lok>&Sen; 15:18</lok></fnt>;
      </ekz>
      <ekz>
        pasanto, kiu sin miksas en malpropran <tld/>on
        <fnt><bib>MT</bib>, <lok>&Sen; 26:17</lok></fnt>;
      </ekz>
      <ekz>
        havi <ind>ne<tld/>ebla</ind>n a&ubreve;toritatecon<fnt>Z</fnt>.
        <trd lng="fr">indiscutable</trd>
      </ekz>  
    </dif> 
    <refgrp tip="vid">
      <ref cel="bek.kun0igxi">kunbeki&gcirc;i</ref>,
      <ref cel="kverel.0i">kvereli</ref>,
      <ref cel="polemi.0i">polemiki</ref>,
      <ref cel="konfli.0i">konflikti</ref>,
      <ref cel="kontes.0i">kontesti</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_y;&c_s;&c_p;&c_u;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_r;&c_a;&c_ch;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">p&rcaron;&iacute;t se</trd>
  <trd lng="de">streiten</trd>
  <trd lng="en">dispute</trd>
  <trd lng="es">disputar</trd>
  <trdgrp lng="fr">
    <trd>se <ind>disputer</ind></trd>,
    <trd>se <ind>quereller</ind></trd></trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_o;&c_r;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_p;&c_i;&c_r;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_d;&c_i;&c_s;&c_p;&c_u;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="disput.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Akre diskuti pri io.  
    </dif>
  </snc>
</drv>
</art>
