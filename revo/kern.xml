<drv mrk="kern.atom0o">
  <kap>atom<tld/>o</kap>
  <snc>
    <uzo tip="fak">FIZ</uzo>
    <dif>
      <tld lit="K"/>o de la <ref tip="malprt" cel="atom.0o">atomo</ref>, 
      konsistanta el unu a&ubreve; <ref tip="prt" cel="proton.0o">protono</ref>j kaj eble 
      <ref tip="prt" cel="neuxtr2.0o">ne&ubreve;trono</ref>j:
      <ekz>
        alfa-radiado konsistas el rapidflugaj atom<tld/>oj de heliumo
        <fnt>
          <bib>Monato</bib>
          <aut>Pejno Simono</aut>
          <vrk><url ref="&Monato;/2011/010370.php">Moderna minaco: radioaktiveco</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        atom<tld/>a energio
        <fnt>
          <bib>MD11</bib>
          <vrk>Malkresko en Okcidento, disetendi&gcirc;o en la sojlolandoj de Azio</vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="nukle.0o.FIZ">nukleo<sncref/></ref>
  </snc>
  <trd lng="be">
  &c_a;&c_t;&c_a;&c_m;&c_n;&c_a;&c_je; &c_ja;&c_d;&c_r;&c_o;</trd>
  <trd lng="cs">atomov&eacute; j&aacute;dro</trd>
