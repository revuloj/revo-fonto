  <trd lng="es">n&uacute;cleo at&oacute;mico</trd>
  <trd lng="fr">noyau atomique</trd>
  <trd lng="he">&gimel;&resh;&ayin;&yod;&fnun; &alef;&tet;&vav;&mem;&yod;</trd>
  <trd lng="hu">atommag</trd>
  <trdgrp lng="id">
    <trd><ind>inti</ind> atom</trd>,
    <trd>nukleus</trd>
  </trdgrp>
  <trd lng="it">nucleo<klr> atomico</klr></trd>
  <trd lng="nl">atoomkern</trd>
  <trd lng="pl">j&aogonek;dro atomowe</trd>
  <trd lng="ru">
