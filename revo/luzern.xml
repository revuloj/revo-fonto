<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>luzern</rad>/o
</kap>
<drv mrk="luzern.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="medika.0o">medikago</ref>
      (<trd lng="la">Medicago sativa</trd>), kun violkoloraj floroj.
    </dif>
  </snc>
  <bld lok="../bld/luzern.jpg">floroj kaj folioj</bld>
  <trd lng="be">&c_l;&c_ju;&c_c;&c_e;&c_r;&c_n;&c_a;</trd>
  <trd lng="de">Luzerne</trd>
  <trd lng="el">&my;&eta;&delta;&jota;&kappa;&eta_ton;</trd>
  <trd lng="fa">&f_y;&a_w;&a_n;&a_j;&a_h;</trd>
  <trd lng="hu">lucerna</trd>
  <trdgrp lng="nl">
    <trd>luzerne</trd>,
    <trd>rupsklaver</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>alfafa</trd>,
    <trd>luzerna</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_ju;&c_c;&c_je;&c_r;&c_n;&c_a;</trd>
</drv>
</art>
