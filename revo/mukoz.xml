<vortaro>
<art mrk="$Id: mukoz.xml,v 1.26 2025/04/04 15:12:27 revo Exp $">
<kap>
  <rad>mukoz</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="mukoz.0o">
  <kap><tld/>o</kap>
  <snc mrk="mukoz.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Malseketa epitelia histo, kiu tegas kelkajn kavojn a&ubreve; duktojn de
      la organismo (digesta, spira, uro-genera organaroj, konjunktivo):
      <ekz>
        afto estas sendan&gcirc;era, sed dolora ulcereto de la bu&scirc;a <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=20417">Afto</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        en popola kuracado <klr>[&#8230;]</klr> valorajn servojn faras pomoj &ccirc;e malvarmumo,
        tuso, laringomalsanoj ktp., la pomo bonege desinfektas la bu&scirc;an kavon, kaj
        varmega kuirita pomsuko kun limono protektas la <tld/>on kontra&ubreve; mukoj
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2014/011362.php">Simbolo de juneco</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>mukmembrano</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_l;&c_ib;&c_z;&c_mol;&c_n;&c_ib;&c_c;&c_a;</trd>,
    <trd>&c_s;&c_mol;&c_l;&c_ib;&c_z;&c_mol;&c_n;&c_je;&c_v;&c_a;&c_ja; &c_a;&c_b;&c_a;&c_l;&c_o;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_l;&c_i;&c_g;&c_a;&c_v;&c_i;&c_c;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_m;&c_u;&c_k;&c_o;&c_z;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">mucosa</trd>
  <trd lng="cs">sliznice</trd>
  <trdgrp lng="de">
    <trd>Schleimhaut</trd>,
    <trd>Mukosa</trd>
  </trdgrp>
  <trd lng="en">mucous membrane</trd>
  <trdgrp lng="es">
    <trd>mucosa</trd>
    <trd>membrana <ind>mucosa</ind></trd>
  </trdgrp>
  <trd lng="fa">&a_gh;&a_sh;&a_A;&a_hamza; &a_m;&a_kh;&a_A;&a_T;&f_y;</trd>
  <trd lng="fr">muqueuse</trd>
  <trd lng="it">mucosa</trd>
  <trd lng="ja">&#31896;&#33180;  <pr>&#12397;&#12435;&#12414;&#12367;</pr></trd>
  <trd lng="nl">slijmvlies</trd>
  <trd lng="pl">b&lstroke;ona &sacute;luzowa </trd>
  <trd lng="pt">mucosa</trd>
  <trd lng="ru">&c_s;&c_l;&c_i;&c_z;&c_i;&c_s;&c_t;&c_a;&c_ja; &c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a;</trd>
  <trd lng="sk">sliznica</trd>
  <trd lng="uk">&#1089;&#1083;&#1080;&#1079;&#1086;&#1074;&#1072; &#1086;&#1073;&#1086;&#1083;&#1086;&#1085;&#1082;&#1072;</trd>
  <trdgrp lng="zh">
    <trd>&#31896;&#33180; <pr>ni&#225;nm&#243;</pr></trd>,
    <trd>&#40655;&#33180; <pr>ni&#225;nm&#242;</pr></trd>
  </trdgrp>
</drv>
</art>
