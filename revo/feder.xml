<kap>
  <ofc>8</ofc>
  <rad>feder</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="feder.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="feder.0i.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
     <ref tip="dif" cel="federa.0i">Federacii</ref>:
      <ekz>
        li postulas <tld/>igon de Serbio sed rifuzas &gcirc;in al Kosovo
        <fnt>
          <bib>Monato</bib>
          <aut>Katoo Ivao</aut>
          <vrk><url ref="&Monato;/2003/008176.html">La junia numero</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;'&c_ja;&c_d;&c_n;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;
    &c_u; <ind>&c_f;&c_e;&c_d;&c_e;&c_r;&c_a;&c_c;&c_y;&c_ju;</ind></trd>
  <trdgrp lng="cs">
    <trd>federalizovat</trd>,
    <trd>vytvo&rcaron;it federaci</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>f&ouml;derieren</trd>,
    <trd>zu einem Bund vereinigen</trd>
  </trdgrp>
  <trd lng="en">federate</trd>
  <trd lng="fr">f&eacute;d&eacute;rer</trd>
  <trdgrp lng="hu">
    <trd>sz&ouml;vets&eacute;gben egyes&iacute;t</trd>,
    <trd>f&ouml;deraliz&aacute;l</trd>
  </trdgrp>
  <trd lng="it">federare</trd>
  <trdgrp lng="ja">
    <trd>&#x9023;&#x5408;&#x3055;&#x305B;&#x308B;  <pr>&#x308C;&#x3093;&#x3054;&#x3046;&#x3055;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x9023;&#x90A6;&#x306B;&#x3059;&#x308B;  <pr>&#x308C;&#x3093;&#x307D;&#x3046;&#x306B;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">federeren</trd>
  <trd lng="pt">federar</trd>
  <trd lng="ru">&c_o;&c_b;&c_malmol;&c_je;&c_d;&c_i;&c_n;&c_ja;&c_t;&c_mol;
&c_v; &c_f;&c_je;&c_d;&c_je;&c_r;&c_a;&c_c;&c_i;&c_ju;</trd>
  <trd lng="sk">vytvori&tcaron; feder&aacute;ciu</trd>
  <trdgrp lng="uk">
    <trd>&#x43E;&c_b;&#x2019;&c_jeu;&c_d;&#x43D;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&c_u;&c_t;&c_v;&#x43E;&c_r;&#x44E;&c_v;&c_a;&c_t;&c_i; &c_f;&c_je;&c_d;&c_je;&c_r;&c_a;&c_c;&c_ib;&#x44E;</trd>,
    <trd>&c_f;&c_je;&c_d;&c_je;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

