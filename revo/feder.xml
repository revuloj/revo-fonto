<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>feder</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="feder.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="feder.0i.POL">
    <uzo tip="fak">POL</uzo>
    <ref tip="dif" cel="federa.0i">federacii</ref>
    <ekz>
      Belgujo estas nia <tld/>ito.
    </ekz>
  </snc>
  <trd lng="be">&c_a;&c_b;'&c_ja;&c_d;&c_n;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;
&c_u; <ind>&c_f;&c_e;&c_d;&c_e;&c_r;&c_a;&c_c;&c_y;&c_ju;</ind></trd>
  <trdgrp lng="de">
    <trd>f&ouml;derieren</trd>,
    <trd>zu einem Bund vereinigen</trd>
  </trdgrp>
  <trd lng="en">federate</trd>
  <trd lng="fr">f&eacute;d&eacute;rer</trd>
  <trdgrp lng="hu">
    <trd>sz&ouml;vets&eacute;gben egyes&iacute;t</trd>,
    <trd>f&ouml;deraliz&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">federeren</trd>
  <trd lng="pt">federar</trd>
  <trd lng="ru">&c_o;&c_b;&c_malmol;&c_je;&c_d;&c_i;&c_n;&c_ja;&c_t;&c_mol;
&c_v; &c_f;&c_je;&c_d;&c_je;&c_r;&c_a;&c_c;&c_i;&c_ju;</trd>
</drv>

<drv mrk="feder.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POL</uzo>
  <ref tip="dif" cel="federa.0o">federacio</ref>
  <trd lng="be">&c_f;&c_e;&c_d;&c_e;&c_r;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>F&ouml;deration</trd>,
    <trd>Staatenbund</trd>
  </trdgrp>
  <trd lng="en">federation</trd>
  <trd lng="es">federaci&oacute;n</trd>
  <trd lng="fr">f&eacute;d&eacute;ration</trd>
  <trdgrp lng="hu">
    <trd>sz&ouml;vets&eacute;g</trd>,
    <trd>&aacute;llamsz&ouml;vets&eacute;g</trd>,
    <trd>f&ouml;der&aacute;ci&oacute;</trd>
  </trdgrp>
  <trd lng="nl">federatie</trd>
  <trd lng="ru">&c_f;&c_je;&c_d;&c_je;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
</drv>

<drv mrk="feder.0ismo">
  <kap><tld/>ismo</kap>
  <ref tip="dif" cel="federa.0ismo.POL">federaciismo</ref>
  <ekz>
    la nocio pri <tld/>ismo ne naski&gcirc;is en la tempo de Wilhelm Tell,
    kiel la svisa folkloro &scirc;atas aserti
    <fnt>
      <aut>C. Gacond</aut>,
      <vrk><url
ref="http://www.esperanto-gacond.ch/Claude_Gacond_Radioprelego_0153.pdf">
      Svisa federismo</url></vrk>,
      <lok>radioprelego, 1966-09</lok>
    </fnt>.
  </ekz>
  <trd lng="en">federalism</trd>
  <trd lng="fr">f&eacute;d&eacute;ralisme</trd>
</drv>
</art>
