<vortaro>
<art mrk="$Id$">
<kap>
<ofc>8</ofc>
<rad>feder</rad>/i
</kap>
<drv mrk="feder.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="dif" cel="federa.0i">Federacii</ref>: 
      <ekz>Belgujo estas nia <tld/>ito.</ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>f&ouml;derieren</trd>,
     <trd>zu einem Bund vereinigen</trd></trdgrp>
  <trd lng="en">federate</trd>
  <trd lng="fr">f&eacute;d&eacute;rer</trd>
  <trd lng="nl">federeren</trd>
  <trd lng="ru">&c_o;&c_b;&c_malmol;&c_je;&c_d;&c_i;&c_n;&c_ja;&c_t;&c_mol; &c_v; &c_f;&c_je;&c_d;&c_je;&c_r;&c_a;&c_c;&c_i;&c_ju;</trd>
</drv>
<drv mrk="feder.0o">
  <kap><tld/>o</kap>
  <snc>
    <ref tip="dif" cel="federa.0o">federacio.</ref>  
  </snc>
  <trdgrp lng="de">
     <trd>F&ouml;deration</trd>,
     <trd>Staatenbund</trd></trdgrp>
  <trd lng="en">federation</trd>
  <trd lng="es">federaci&oacute;n</trd>
  <trd lng="fr">f&eacute;d&eacute;ration</trd>
  <trd lng="nl">federatie</trd>
  <trd lng="ru">&c_f;&c_je;&c_d;&c_je;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
</drv>
</art>

