<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: for.xml,v 1.49 2021/05/28 12:14:58 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>for</rad> <fnt><bib>PV</bib></fnt>
</kap>
<subart>
  <drv mrk="for.0">
    <kap><tld/></kap>
    <dif>
      Adverbo signifanta:
    </dif>
    <snc mrk="for.0.malapero">
      <dif>
        Malaperon kaj perdi&gcirc;on en malproksimo:
        <ekz>
          <tld/> de l' okuloj, <tld/> de la koro
          <fnt><bib>PrV</bib></fnt>;
        </ekz><ekz>
          pasinta doloro <tld/> el memoro
          <fnt><bib>PrV</bib></fnt>;
        </ekz><ekz>
          <tld/> vi(n)!
        </ekz><ekz>
          <tld/> la manojn!
        </ekz><ekz>
          <tld/> la revoj(n)!
        </ekz><ekz>
          <tld/> plezuro(n)!
        </ekz><ekz>
          &ccirc;iu espero estas <tld/>;
        </ekz><ekz>
          la digo estis <tld/> kaj la torento povis libere elfluegi.
        </ekz>
      </dif>
      <ref tip="sin" cel="for.0en"><tld/>en</ref>
      <trd lng="en">away</trd>
      <trd lng="es">fuera</trd>
      <trdgrp lng="fi">
        <trd>pois</trd>,
        <trd>poissa</trd>
      </trdgrp>
      <trdgrp lng="pt">
        <trd>longe</trd>,
        <trd>fora</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_p;&c_r;&c_o;&c_ch;&c_mol;</trd>,
        <trd>&c_v;&c_o;&c_n;</trd>,
        <trd>&c_d;&c_o;&c_l;&c_o;&c_j;</trd>
      </trdgrp>
    </snc>
    <snc mrk="for.0.malproksimeco">
      <dif>
        Malproksimecon:
        <ekz>
          Marta staris jam <ind>kelke da pa&scirc;oj <tld/> de
          &scirc;i</ind><fnt>Z</fnt>
          <trd lng="fi">muutaman askelen <ind>p&auml;&auml;ss&auml;</ind>
             h&auml;nest&auml;</trd>;
        </ekz><ekz>
          ve al tiu, kiu devas vivi <tld/> de la gepatroj kaj de la
          gefratoj<fnt>Z</fnt>;
        </ekz><ekz>
          sur la kampo, <tld/> de l' mondo<fnt>Z</fnt>;
        </ekz><ekz>
          fikse rigardantaj <tld/> anta&ubreve;en sur la nigra oceano;
        </ekz><ekz>
          la libretoj estis dissenditaj <tld/> en la mondon;
        </ekz><ekz>
          tio estas <tld/> de mia intenco.
        </ekz>
      </dif>
      <ref tip="sin" cel="for.0e"><tld/>e</ref>
      <trd lng="en">far</trd>
      <trd lng="es">lejos</trd>
      <trdgrp lng="fi">
        <trd>kaukana</trd>,
        <trd>et&auml;&auml;ll&auml;</trd>,
        <trd>kauas</trd>
      </trdgrp>
      <trdgrp lng="pt">
        <trd>longe</trd>,
        <trd>fora</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_v;&c_d;&c_a;&c_l;&c_je;&c_k;&c_je;<klr> (&c_o;&c_t; &c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>,
        <trd>&c_v;&c_d;&c_a;&c_l;&c_i;<klr> (&c_o;&c_t; &c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>,
        <trd>&c_d;&c_a;&c_l;&c_je;&c_k;&c_o;<klr> (&c_o;&c_t; &c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>
      </trdgrp>
    </snc>
    <snc mrk="for.0.malcxi">
      <uzo tip="stl">RAR</uzo>
      <dif>
        <ref tip="dif" cel="cxi.mal0">Mal&ccirc;i</ref>:
        <ekz>
          du &ccirc;evaloj, tiu &ccirc;i kaj tiu <tld/>
          <fnt><bib>PAG</bib>, <lok>&para;54, Rim.V</lok></fnt>;
        </ekz><ekz>
          Ku&scirc;as vestoj de viro tie <tld/>, iu viro banas sin tute apude
          <fnt>
            <aut>Hjalmar S&ouml;derberg, tr. Sten Johansson</aut>:
            <vrk>La bovinoj de la paro&hcirc;estro</vrk>.
            <lok>La Kiso Kaj dek tri aliaj noveloj, 1955</lok>
            </fnt>.
        </ekz>
      </dif>
      <rim>
        Ne konfuzu &ccirc;i tian <tld/><sncref ref="for.0.malcxi"/>
        kun <tld/><sncref ref="for.0.malapero"/>:
        <ekz>
          Forpelu de mi &ccirc;i tiun <tld/><sncref ref="for.0.malapero"/>,
          kaj &scirc;losu la pordon post &scirc;i!
          <fnt><bib>MT</bib>, <lok>2Sam 13:17</lok></fnt>.
        </ekz>
      </rim>
    </snc>
    <trd lng="bo">&#xF55;&#xF62;&#xF0B;</trd>
  <trd lng="cs">pry&ccaron;</trd>
  <trdgrp lng="de">
      <trd>weg</trd>, <trd>fort</trd>, <trd>entfernt</trd>,
      <trd>weit</trd>
  </trdgrp>
  <trd lng="fr">loin</trd>
  <trdgrp lng="nl">
      <trd>weg</trd>,
      <trd>uit</trd>
    </trdgrp>
  <trd lng="sk">pre&ccaron;</trd>
</drv>
</subart>

<subart>
  <dif>
    Vortero signifanta malproksimecon a&ubreve; malaperon (kaj eble
    nenii&gcirc;on):
  </dif>

  <drv mrk="for.0a">
    <kap><tld/>a</kap>
    <snc mrk="for.0a.KOMUNE">
      <dif>
        <ref tip="sin" cel="proksi.mal0a">Malproksima</ref>:
        <ekz>
          li lo&gcirc;as en <tld/>a angulo de Siberio;
        </ekz><ekz>
          nia plej <tld/>a kolonio;
        </ekz><ekz>
          la <tld/>a horizonto;
        </ekz><ekz>
          <tld/>aj veloj glitis sur la maro;
        </ekz><ekz>
          <tld/>a estonteco;
        </ekz><ekz>
          en seskilometra <tld/>eco trovi&gcirc;as ruinoj.
        </ekz>
      </dif>
      <trd lng="de">abgelegen</trd>
      <trd lng="es">lejano</trd>
      <trdgrp lng="fi">
        <trd>kaukainen</trd>,
        <trd>et&auml;inen</trd>
      </trdgrp>
      <trd lng="nl">afgelegen</trd>
    </snc>

    <snc mrk="for.0a.TEK">
      <uzo tip="fak">TEK</uzo>
      <uzo tip="fak">KOMP</uzo>
      <dif>
        Pri komputilo a&ubreve; aran&gcirc;a&jcirc;o atingata per komunika lineo:
        <ekz>
          <uzo tip="fak">KOMP</uzo>
          <ind><tld/>a terminalo</ind>;
          <trd lng="de">entfernt</trd>
          <trd lng="en">remote terminal</trd>
          <trd lng="ru">&c_u;&c_d;&c_a;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j; &c_t;&c_je;&c_r;&c_m;&c_i;&c_n;&c_a;&c_l;</trd>
        </ekz><ekz>
          <uzo tip="fak">KOMP</uzo>
          <ind><tld/>a uzanto</ind>;
          <trd lng="en">remote user</trd>
          <trd lng="es">usuario remoto</trd>
          <trd lng="ru">&c_d;&c_i;&c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j; &c_p;&c_o;&c_l;&c_mol;&c_z;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
        </ekz>
      </dif>
      <trd lng="en">remote</trd>
      <trd lng="es">remoto</trd>
      <trd lng="fi">et&auml;-</trd>
      <trd lng="ru">&c_d;&c_i;&c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
    </snc>
  <trd lng="bo">&#xF50;&#xF42;&#xF0B;&#xF62;&#xF92;&#xFB1;&#xF44;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>d&aacute;lkov&yacute;</trd>,
    <trd>na d&aacute;lku</trd>,
    <trd>vzd&aacute;len&yacute;</trd>
  </trdgrp>
  <trdgrp lng="fr">
      <trd>lointain<klr tip="ind"> (adj.)</klr></trd>,
      <trd>&eacute;loign&eacute;</trd>
    </trdgrp>
  <trd lng="nl">afstands-</trd>
  <trdgrp lng="ru">
      <trd>&c_d;&c_a;&c_l;&c_jo;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_u;&c_d;&c_a;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_o;&c_t;&c_d;&c_a;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  <trdgrp lng="sk">
    <trd>d&aacute;vny</trd>,
    <trd>vzdialen&yacute;</trd>
  </trdgrp>
</drv>

  <drv mrk="for.0e">
    <kap><tld/>e</kap>
    <snc>
      <dif>
        Malproksime (senambigua formo de
        <ref tip="sin" cel="for.0.malproksimeco"><tld/><sncref/></ref>):
        <ekz>
          mi lo&gcirc;as pli <tld/>e;
        </ekz><ekz>
          <ind>la stacidomo estas <tld/>e je duonhoro</ind>
          <trd lng="fi">asema on puolen tunnin (matkan) p&auml;&auml;ss&auml;</trd>;
        </ekz><ekz>
          li etendis al la amiko la brakojn de <tld/>e.
        </ekz>
      </dif>
    </snc>
  <trdgrp lng="cs">
    <trd>daleko</trd>,
    <trd>pry&ccaron;<klr> (kde?)</klr></trd>,
    <trd>v d&aacute;lce</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>abgelegen</trd>,
      <trd>j.w.d.</trd>,
      <trd>jwd</trd>
    </trdgrp>
  <trdgrp lng="es">
    <trd>lejanamente</trd>
  </trdgrp>
  <trdgrp lng="fi">
      <trd>kaukana</trd>
    </trdgrp>
  <trdgrp lng="fr">
      <trd>loin</trd>,
      <trd>au <ind>loin</ind></trd>
    </trdgrp>
  <trd lng="nl">ver</trd>
  <trdgrp lng="ru">
      <trd>&c_d;&c_a;&c_l;&c_je;&c_k;&c_o;</trd>,
      <trd>&c_v;&c_d;&c_a;&c_l;&c_i;</trd>
    </trdgrp>
  <trd lng="sk">v dia&lcaron;ke</trd>
</drv>

  <drv mrk="for.0en">
    <kap><tld/>en</kap>
    <snc>
      <dif>
        Malproksimen (senambigua formo de
        <ref tip="sin" cel="for.0.malapero"><tld/><sncref/></ref>):
        <ekz>
          ni rajdas multe da mejloj <tld/>en
          <fnt><bib>Fab2</bib>, <lok>Sambuka virineto</lok></fnt>;
        </ekz><ekz>
          ili diskuris <tld/>en;
        </ekz><ekz>
          voja&gcirc;i <tld/>en;
        </ekz><ekz>
          tiun demandon li ju&gcirc;as de punkto tre alta kaj <tld/>en
          videbliganta.
        </ekz>
      </dif>
    </snc>
  <trdgrp lng="cs">
    <trd>daleko</trd>,
    <trd>do daleka</trd>
  </trdgrp>
  <trd lng="de">weg</trd>
  <trdgrp lng="es">
    <trd>a lo lejos</trd>  
  </trdgrp>
  <trdgrp lng="fi">
      <trd>kauas</trd>,
      <trd>pitk&auml;lle</trd>
  </trdgrp>
  <trdgrp lng="fr">
      <trd>loin</trd>,
      <trd>au <ind>loin</ind></trd>
    </trdgrp>
  <trd lng="nl">ver</trd>
  <trd lng="ru">&c_v;&c_d;&c_a;&c_l;&c_mol;</trd>
  <trd lng="sk">&dcaron;aleko</trd>
</drv>

  <drv mrk="for.0ajxo">
    <kap><tld/>a&jcirc;o<fnt>K</fnt></kap>
    <snc>
      <dif>
        Malproksima&jcirc;o:
        <ekz>
          li malaperis en la <tld/>a&jcirc;o.
        </ekz>
      </dif>
    </snc>
    <trd lng="es">lejan&iacute;a</trd>
    <trdgrp lng="fi">
      <trd>kaukaisuus</trd>,
      <trd>et&auml;inen paikka</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>lointain<klr tip="ind"> (subst.)</klr></trd>,
      <trd>horizon<klr tip="ind"> (le lointain)</klr></trd>,
      <trd>arri&egrave;re-plan</trd>
    </trdgrp>
    <trd lng="nl">verte</trd>
    <trd lng="ru">&c_d;&c_a;&c_l;&c_mol;</trd>
  </drv>

  <drv mrk="for.00ejo">
    <kap><tld/><tld/>ejo</kap>
    <snc mrk="for.00ejo.KOMUNE">
      <uzo tip="stl">KOMUNE</uzo>
      <dif>
        Tre malproksima, tre izolita, nekonata loko:
        <ekz>
          plikreskantaj kostoj de la benzino <klr>...</klr> dolore 
          senteblas speciale en la dezertaj regionoj, <tld/><tld/>ejoj, 
          kaj e&ccirc; grandurbaj regionoj de A&ubreve;stralio
          <fnt>
            <aut>Z. Wybraniec</aut>,
            <vrk><url ref="http://lapraktiko.blogspot.fr/2013/01/fare-de-zbigniewwybraniec-alternativa.html">
            Nomadula Vivo 2</url></vrk>,
            <lok>2013-01-10</lok>
          </fnt>.
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="es">
      <trd>en un lugar muy lejano</trd>
      <trd>(en) el quinto pino<klr> (expresi&oacute;n)</klr></trd>
    </trdgrp>
    <trd lng="fr">diable vauvert</trd>
  </drv>

  <drv mrk="for.0igi">
    <kap><tld/>igi</kap>
    <snc>
      <dif>
        Malaperigi, neniigi:
        <ekz>
          <tld/>igi makulon, bre&ccirc;on, mankon;
        </ekz><ekz>
          multaj &ccirc;agrenoj kaj suferoj povus esti <tld/>igataj,
          se ...<fnt>Z</fnt>;
        </ekz><ekz>
          por <tld/>igi &ccirc;iujn
          dubojn<fnt>Z</fnt>, embarasojn<fnt>Z</fnt>
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="ekster1.0i">ekstermi</ref>,
        <ref cel="likvid.0i">likvidi</ref>,
        <ref cel="visx.for0i">forvi&scirc;i</ref>.
      </refgrp>
    </snc>
    <trdgrp lng="cs">
      <trd>odebrat</trd>,
      <trd>odklidit</trd>,
      <trd>odlo&zcaron;it</trd>,
      <trd>odstranit</trd>,
      <trd>vymazat</trd>,
      <trd>vyndat</trd>,
      <trd>vy&rcaron;adit</trd>,
      <trd>zahladit</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>entfernen</trd>,
      <trd><ind>weg</ind> machen</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>suprimir</trd>,
      <trd>eliminar</trd>,
      <trd>quitar</trd>
    </trdgrp>
    <trdgrp lng="fi">
      <trd>poistaa</trd>,
      <trd>ottaa pois</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>enlever</trd>,
      <trd>&eacute;loigner</trd>,
      <trd>&eacute;carter<klr tip="ind"> (&eacute;loigner)</klr></trd>,
      <trd>&ocirc;ter</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>verwijderen</trd>,
      <trd><ind>weg</ind> doen</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_u;&c_d;&c_a;&c_l;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_u;&c_b;&c_r;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_s;&c_t;&c_r;&c_a;&c_n;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_u;&c_s;&c_t;&c_r;&c_a;&c_n;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="sk">odstr&aacute;ni&tcaron;</trd>
  </drv>

  <drv mrk="for.0igxi">
    <kap><tld/>i&gcirc;i<fnt>Z</fnt></kap>
    <snc>
      <dif>
        Malaperi, nenii&gcirc;i:
        <ekz>
          iom post iom re<tld/>i&gcirc;is la akvo de sur la tero
          <fnt><bib>MT</bib>, <lok>&Gen; 8:3</lok></fnt>.
        </ekz>
      </dif>
    </snc>
    <trd lng="es">alejarse</trd>
    <trdgrp lng="fi">
      <trd>poistua</trd>,
      <trd>h&auml;vit&auml;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>se <ind>retirer</ind></trd>,
      <trd>s'<ind>&eacute;loigner</ind></trd>
    </trdgrp>
    <trd lng="de">verschwinden</trd>
    <trdgrp lng="nl">
      <trd>verdwijnen</trd>,
      <trd><ind>weg</ind> gaan</trd>
    </trdgrp>
    <trd lng="ru">&c_u;&c_d;&c_a;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </drv>

  <drv mrk="for.de0a">
    <kap>de<tld/>a</kap>
    <snc mrk="for.de0a.TEK">
      <uzo tip="fak">TEK</uzo>
      <uzo tip="fak">KOMP</uzo>
      <dif>
        <ref tip="super" cel="for.0a.TEK">Fora</ref> kaj tia, ke
        la &ccirc;efaganto agas el loko alia ol la koncerna agadejo:
        <ekz>
          <uzo tip="fak">KOMP</uzo>
          <ind>de<tld/>a atingo</ind>
          <klr>(uzado de risurcoj de unu komputilo pere de alia
          komputilo, ekz-e per Telnet)</klr>;
          <trd lng="en">remote access</trd>
          <trd lng="es">acceso remoto</trd>
          <trd lng="ru">&c_u;&c_d;&c_a;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j; &c_d;&c_o;&c_s;&c_t;&c_u;&c_p;</trd>
        </ekz><ekz>
          <uzo tip="fak">TEK</uzo>
          <ind>de<tld/>a stirado</ind>;
          <trd lng="de">Fernsteuerung</trd>
          <trd lng="en">remote control</trd>
          <trd lng="es">control remoto</trd>
          <trd lng="nl">afstandsbediening</trd>
          <trd lng="ru">&c_d;&c_i;&c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_o;&c_n;&c_n;&c_o;&c_je; &c_u;&c_p;&c_r;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>aus der <ind>Ferne</ind></trd>,
        <trd>von <ind>ferne</ind></trd>,
        <trd>von <ind>weitem</ind></trd>,
        <trd>entfernt</trd>
      </trdgrp>
      <trd lng="en">remote</trd>
      <trdgrp lng="es">
        <trd>de lejos</trd>,
        <trd>remoto</trd>
      </trdgrp>
      <trdgrp lng="fi">
        <trd>kaukaa tapahtuva</trd>,
        <trd>et&auml;-</trd>,
        <trd>kauko-</trd>
      </trdgrp>
      <trd lng="nl">afstands-</trd>
      <trd lng="ru">&c_d;&c_i;&c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
    </snc>
    <trd lng="he">&#x5E8;&#x5D7;&#x5D5;&#x5E7;</trd>
</drv>

  <drv mrk="for.antaux0igi">
    <kap>anta&ubreve;<tld/>igi</kap>
    <snc>
      <dif>
        Eviti, malebligi per anta&ubreve;a singardo:
        <ekz>
          anta&ubreve;<tld/>igi akcidenton.
        </ekz>
      </dif>
      <refgrp tip="vid">
        Preska&ubreve;aj sinonimoj:
        <ref cel="help.antauxmal0i">anta&ubreve;malhelpi</ref>,
        <ref cel="halt.antaux0igi">anta&ubreve;haltigi</ref>.
      </refgrp>
    </snc>
    <trd lng="de">vorsorglich <ind>verhindern</ind></trd>
    <trdgrp lng="es">
      <trd>prevenir</trd>,
      <trd>evitar</trd>
    </trdgrp>
    <trdgrp lng="fi">
      <trd>poistaa ennakolta</trd>,
      <trd>ehk&auml;ist&auml;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>emp&ecirc;cher<klr tip="ind"> (pr&eacute;venir)</klr></trd>,
      <trd>pr&eacute;venir<klr tip="ind"> (&eacute;viter)</klr></trd>
    </trdgrp>
    <trd lng="nl">verhinderen</trd>
    <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_o;&c_t;&c_v;&c_r;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>
  </drv>

</subart>

<subart>
  <dif>
    Prefikso havanta la sencojn <tld/><sncref ref="for.0.malapero"/>
    a&ubreve; <tld/><sncref ref="for.0.malproksimeco"/>, sed plej ofte la
    unuan:
  </dif>
  <drv mrk="for.0akrigi">
    <kap><tld/>akrigi</kap>
    <snc>
      <dif>
        <tld lit="F"/>igi akrigante:
        <ekz>
          <tld/>akrigi la bre&ccirc;etojn<fnt>Z</fnt>.
        </ekz>
      </dif>
    </snc>
    <trd lng="es">limar</trd>
    <trd lng="ru">&c_s;&c_t;&c_o;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </drv>
  <drv mrk="for.0acxeti">
    <kap><tld/>a&ccirc;eti</kap>
    <snc>
      <dif>
        Malaperigi la varojn el la merkato a&ccirc;etante &ccirc;iom.
      </dif>
    </snc>
    <trdgrp lng="de">
      <trd>aufkaufen</trd>,
      <trd>leerkaufen</trd>,
      <trd>wegkaufen</trd>
    </trdgrp>
    <trd lng="es">desvalijar</trd>
    <trd lng="fr">d&eacute;valiser<klr tip="ind"> (tout acheter)</klr></trd>
    <trd lng="nl">opkopen</trd>
    <trd lng="ru">&c_s;&c_k;&c_u;&c_p;&c_i;&c_t;&c_mol;<klr> (&c_v;&c_je;&c_s;&c_mol; &c_t;&c_o;&c_v;&c_a;&c_r;)</klr></trd>
  </drv>
  <drv mrk="for.0argumenti">
    <kap><tld/>argumenti</kap>
    <snc>
      <dif>
        Argumentante senpravigi kaj senvalorigi.
      </dif>
    </snc>
    <trd lng="de"><klr tip="ind">(jmd. etwas)</klr> ausreden</trd>
    <trd lng="es">replicar</trd>
    <trd lng="fr">r&eacute;pliquer</trd>
    <trd lng="nl">uit het hoofd <ind>praten</ind></trd>
  </drv>
  <drv mrk="for.0beni">
    <kap><tld/>beni</kap>
    <snc>
      <dif>
        <tld lit="F"/>igi per beno:
        <ekz>
          <tld/>beni al iu la pekojn<fnt>Z</fnt>.
        </ekz>
      </dif>
    </snc>
    <ref cel="absolv.0i">absolvi</ref>
    <trd lng="cs">za&zcaron;ehnat</trd>
    <trdgrp lng="es">
      <trd>absolver</trd>,
      <trd>perdonar</trd>
    </trdgrp>
    <trd lng="fr">absoudre</trd>
    <trd lng="nl">vergeven</trd>
    <trd lng="ru">&c_o;&c_t;&c_p;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol;<klr tip="amb">
    (&c_g;&c_r;&c_je;&c_h;&c_i;)</klr></trd>
    <trd lng="sk">za&zcaron;ehna&tcaron;</trd>
</drv>
  <drv mrk="for.0danci0labori0ludi">
    <kap><tld/>danci, <var><kap><tld/>labori</kap></var>,
    <var><kap><tld/>ludi</kap></var></kap>
    <snc>
      <dif>
        <tld lit="F"/>igi, perdi, forgesi, dancante, laborante ktp:
      </dif>
    </snc>
  <trd lng="cs">prohr&aacute;t</trd>
  <trd lng="es">perder</trd>
  <trd lng="sk">prehra&tcaron;<klr> (v hre)</klr></trd>
</drv>
  <drv mrk="for.0difekti">
    <kap><tld/>difekti<fnt>Z</fnt></kap>
    <snc>
      <dif>
        Difekti objekton <tld/>igante ion de &gcirc;i.
      </dif>
    </snc>
  <trd lng="es">evitar un da&ntilde;o</trd>
  </drv>
  <drv mrk="for.0direkti">
    <kap><tld/>direkti</kap>
    <snc>
      <dif>
        Deturni:
        <ekz>
          <tld/>direkti sian &ccirc;aron<fnt>Z</fnt>.
        </ekz>
      </dif>
    </snc>
  <trdgrp lng="cs">
    <trd>odklonit<klr> (od sm&ecaron;ru)</klr></trd>,
    <trd>odv&eacute;st</trd>,
    <trd>zav&eacute;st n&ecaron;kam pry&ccaron;</trd>
  </trdgrp>
  <trd lng="es">desviar</trd>
  <trd lng="fr">d&eacute;vier</trd>
  <trd lng="nl">doen <ind>afwijken</ind></trd>
  <trdgrp lng="sk">
    <trd>odkloni&tcaron;</trd>,
    <trd>odvies&tcaron;</trd>
  </trdgrp>
</drv>
  <drv mrk="for.0eskorti">
    <kap><tld/>eskorti</kap>
    <snc>
      <dif>
        <tld/>konduki eskortante.
      </dif>
    </snc>
  <trdgrp lng="cs">
    <trd>odeskortovat</trd>,
    <trd>odv&eacute;st eskortou</trd>
  </trdgrp>
  <trd lng="es">guiar</trd>
  <trd lng="fr">reconduire<klr tip="ind"> (qn chez lui)</klr></trd>
  <trd lng="nl">begeleiden</trd>
  <trdgrp lng="ru">
      <trd>&c_u;&c_v;&c_je;&c_s;&c_t;&c_i;</trd>,
      <trd>&c_v;&c_y;&c_v;&c_je;&c_s;&c_t;&c_i;</trd>,
      <trd>&c_v;&c_y;&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">odeskortova&tcaron;</trd>
</drv>
  <drv mrk="for.0frostigxi">
    <kap><tld/>frosti&gcirc;i</kap>
    <snc>
      <dif>
        Detrui&gcirc;i per frosto.
      </dif>
    </snc>
    <trd lng="es">congelarse<klr tip="ind"> (da&ntilde;arse por
    hielo)</klr></trd>
    <trd lng="fr">geler<klr tip="ind"> (&ecirc;tre abim&eacute; par le
gel)</klr></trd>
    <trd lng="nl">bevriezen<klr tip="ind"> (door ijs
    beschadigen)</klr></trd>
  </drv>
  <drv mrk="for.0froti">
    <kap><tld/>froti</kap>
    <snc>
      <dif>
        <tld lit="F"/>igi frotante.
      </dif>
    </snc>
  <trd lng="cs">set&rcaron;&iacute;t</trd>
  <trd lng="es">fregar</trd>
  <trd lng="fr">r&eacute;curer</trd>
  <trd lng="nl">wegkrabben</trd>
  <trdgrp lng="ru">
      <trd>&c_s;&c_t;&c_je;&c_r;&c_je;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_t;&c_je;&c_r;&c_je;&c_t;&c_mol;</trd>
    </trdgrp>
  <trd lng="sk">vytiera&tcaron;</trd>
</drv>
  <drv mrk="for.0leki">
    <kap><tld/>leki</kap>
    <snc>
      <dif>
        <tld lit="F"/>igi lekante:
        <ekz>
          la fajro <tld/>lekis la akvon<fnt>Z</fnt>.
        </ekz>
      </dif>
    </snc>
  <trd lng="cs">sl&iacute;znout</trd>
  <trd lng="es">lamer</trd>
  <trd lng="fr">tout <ind>nettoyer</ind><klr tip="amb"> (avec la
    langue)</klr></trd>
  <trd lng="nl">weglikken</trd>
  <trd lng="ru">&c_s;&c_l;&c_i;&c_z;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">zl&iacute;za&tcaron;</trd>
</drv>
  <drv mrk="for.0malaperi">
    <kap><tld/>malaperi</kap>
    <snc>
      <dif>
        Tute malaperi.
      </dif>
    </snc>
    <trdgrp lng="de">
      <trd>verschwinden</trd>,
      <trd>sich in Luft aufl&ouml;sen</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd><ind>evaporar</ind>se<klr tip="ind">
(desaparecer)</klr></trd>,
      <trd><ind>volatilizar</ind>se<klr tip="ind">
      (desaparecer)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>s'<ind>&eacute;vaporer</ind><klr tip="ind">
(dispara&icirc;tre)</klr></trd>,
      <trd>se <ind>volatiliser</ind><klr tip="ind">
      (dispara&icirc;tre)</klr></trd>
    </trdgrp>
    <trd lng="nl">in <ind>rook</ind> opgaan</trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_p;&c_a;&c_s;&c_t;&c_mol;</trd>
  </drv>
  <drv mrk="for.0ofendi">
    <kap><tld/>ofendi</kap>
    <snc>
      <dif>
        <tld lit="F"/>pu&scirc;i iun ofendante.
      </dif>
    </snc>
    <trd lng="es">ofender</trd>
    <trd lng="fr">froisser<klr tip="ind"> (qn)</klr></trd>
  </drv>
  <drv mrk="for.0pafi">
    <kap><tld/>pafi</kap>
    <snc>
      <dif>
        <tld lit="F"/>igi pafante:
        <ekz>
          per sia mallerteco li nur <tld/>pafis la birdojn.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>abschie&szlig;en</trd>,
        <trd>umschie&szlig;en</trd>,
        <trd>verscheuchen</trd>
      </trdgrp>
      <trdgrp lng="es">
        <trd>ahuyentar</trd>,
        <trd>espantar</trd>
      </trdgrp>
      <trd lng="fr">effaroucher</trd>
      <trd lng="nl">wegschieten</trd>
      <trd lng="ru">&c_o;&c_t;&c_s;&c_t;&c_r;&c_je;&c_l;&c_i;&c_t;&c_mol;</trd>
    </snc>
  </drv>
  <drv mrk="for.0pafigxi">
    <kap><tld/>pafi&gcirc;i, <var><kap>sin <tld/>pafi</kap></var>,
    <var><kap>pafi sin <tld/></kap></var></kap>
    <snc>
      <dif>
        Subite malaperi:
        <ekz>
          li pafis sin for<fnt><bib>PrV</bib></fnt>.
        </ekz>
      </dif>
    </snc>
    <trd lng="cs">pry&ccaron;</trd>
    <trdgrp lng="de">
      <trd>davonschie&szlig;en</trd>,
      <trd>davonsausen</trd>,
      <trd>davoneilen</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>salir disparado</trd>,
      <trd>despegar<klr> (un cohete)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>se tirer<klr tip="ind"> (s'en aller)</klr></trd>,
      <trd>filer<klr tip="ind"> (s'en aller)</klr></trd>
    </trdgrp>
    <trd lng="nl">wegschieten<klr tip="ind"> (snel weglopen)</klr></trd>
    <trd lng="sk">pre&ccaron;</trd>
  </drv>
  <drv mrk="for.0putri0putrigxi">
    <kap><tld/>putri<fnt>Z</fnt>,
    <var><kap><tld/>putri&gcirc;i<fnt>Z</fnt></kap></var></kap>
    <snc>
      <dif>
        Tute putri&gcirc;i, nenii&gcirc;i a&ubreve; senvalori&gcirc;i
putri&gcirc;ante.
      </dif>
    </snc>
    <trd lng="de">verfaulen</trd>
    <trd lng="es">pudrirse del todo</trd>
    <trd lng="nl">wegrotten</trd>
    <trd lng="ru">&c_s;&c_g;&c_n;&c_i;&c_t;&c_mol;</trd>
  </drv>
  
  <drv mrk="for.0veturi">
    <kap><tld/>veturi<fnt>Z</fnt></kap>
    <snc>
      <dif>
        <tld/>iri veturante.
      </dif>
    </snc>
  <trd lng="cs">odjet</trd>
  <trd lng="de">wegfahren</trd>
  <trd lng="es">partir<klr> (de viaje)</klr></trd>
  <trd lng="fr">partir</trd>
  <trd lng="nl">wegrijden</trd>
  <trd lng="ru">&c_u;&c_je;&c_h;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">odcestova&tcaron;</trd>
</drv>
</subart>
