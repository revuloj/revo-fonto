<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>for</rad></kap>
<subart>
  <dif>
    Adverbo signifanta:
  </dif>
  <snc mrk="for.0.malapero">
    <dif>
      Malaperon kaj perdi&gcirc;on en malproksimo:
      <ekz>
        <tld/> de l' okuloj, <tld/> de la koro<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        pasinta doloro <tld/> el memoro<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/> vi(n)!
      </ekz>
      <ekz>
        <tld/> la manoj(n)!
      </ekz>
      <ekz>
        <tld/> la revoj(n)!
      </ekz>
      <ekz>
        <tld/> plezuro(n)!
      </ekz>
      <ekz>
        &ccirc;iu espero estas <tld/>;
      </ekz>
      <ekz>
        la digo estis <tld/> kaj la torento povis libere elfluegi.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_ch;&c_mol;</trd>,
      <trd>&c_v;&c_o;&c_n;</trd>,
      <trd>&c_d;&c_o;&c_l;&c_o;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="for.0.malproksimeco">
    <dif>
      Malproksimecon:
      <ekz>
        Marta staris jam kelke da pa&scirc;oj <tld/> de
        &scirc;i<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ve al tiu, kiu devas vivi <tld/> de la gepatroj kaj de la
        gefratoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        sur la kampo, <tld/> de l' mondo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        fikse rigardantaj <tld/> anta&ubreve;en sur la nigra oceano;
      </ekz>
      <ekz>
        la libretoj estis dissenditaj <tld/> en la mondon;
      </ekz>
      <ekz>
        tio estas <tld/> de mia intenco.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_v;&c_d;&c_a;&c_l;&c_je;&c_k;&c_je; <klr>(&c_o;&c_t; &c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>,
      <trd>&c_v;&c_d;&c_a;&c_l;&c_i; <klr>(&c_o;&c_t; &c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>,
      <trd>&c_d;&c_a;&c_l;&c_je;&c_k;&c_o; <klr>(&c_o;&c_t; &c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="de">
    <trd>weg</trd>, <trd>fort</trd>, <trd>entfernt</trd>, <trd>weit</trd>
  </trdgrp>
  <trd lng="es">lejos</trd>
  <trd lng="fr">loin</trd>
  <trdgrp lng="nl">
    <trd>weg</trd>,
    <trd>uit</trd>
  </trdgrp>
</subart>

<subart>
  <dif>
    Vortero signifanta malproksimecon a&ubreve; malaperon kaj nenii&gcirc;on:
  </dif>
  <drv mrk="for.0a">
    <kap><tld/>a</kap>
    <snc mrk="for.0a.KOMUNE">
      <dif>
        Malproksima:
        <ekz>
          li lo&gcirc;as en <tld/>a angulo de Siberio;
        </ekz>
        <ekz>
          nia plej <tld/>a kolonio;
        </ekz>
        <ekz>
          la <tld/>a horizonto;
        </ekz>
        <ekz>
          <tld/>aj veloj glitis sur la maro;
        </ekz>
        <ekz>
          <tld/>a estonteco;
        </ekz>
        <ekz>
          en seskilometra <tld/>eco trovi&gcirc;as ruinoj.
        </ekz>
      </dif>
      <trd lng="de">abgelegen</trd>
      <trd lng="nl">afgelegen</trd>
    </snc>
    <snc mrk="for.0a.TEK">
      <uzo tip="fak">TEK</uzo>
      <uzo tip="fak">KOMP</uzo>
      <dif>
        Pri komputilo a&ubreve; aran&gcirc;a&jcirc;o atingata per komunika lineo:
        <ekz>
          <uzo tip="fak">KOMP</uzo>
          <ind><tld/>a terminalo</ind>;
          <trd lng="de">entfernt</trd>
          <trd lng="en">remote terminal</trd>
          <trd lng="ru">&c_u;&c_d;&c_a;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j; &c_t;&c_je;&c_r;&c_m;&c_i;&c_n;&c_a;&c_l;</trd>
        </ekz>
        <ekz>
          <uzo tip="fak">KOMP</uzo>
          <ind><tld/>a uzanto</ind>;
          <trd lng="en">remote user</trd>
          <trd lng="ru">&c_d;&c_i;&c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j; &c_p;&c_o;&c_l;&c_mol;&c_z;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
        </ekz>
      </dif>
      <trd lng="en">remote</trd>
      <trd lng="ru">&c_d;&c_i;&c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
    </snc>
    <trd lng="cs">vzd&aacute;len&yacute;</trd>
    <trd lng="es">lejano</trd>
    <trdgrp lng="fr">
      <trd>lointain<klr tip="ind"> (adj.)</klr></trd>,
      <trd>&eacute;loign&eacute;</trd>
    </trdgrp>
    <trd lng="nl">afstands-</trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_a;&c_l;&c_jo;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_u;&c_d;&c_a;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_o;&c_t;&c_d;&c_a;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </drv>
  <drv mrk="for.0e">
    <kap><tld/>e</kap>
    <snc>
      <dif>
        Malproksime:
        <ekz>
          mi lo&gcirc;as pli <tld/>e;
        </ekz>
        <ekz>
          la stacidomo estas <tld/>e je duonhoro;
        </ekz>
        <ekz>
          li etendis al la amiko la brakojn de <tld/>e.
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="de">
      <trd>abgelegen</trd>,
      <trd>j.w.d.</trd>
      <trd>jwd</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>loin</trd>,
      <trd>au <ind>loin</ind></trd>
    </trdgrp>
    <trd lng="nl">ver</trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_a;&c_l;&c_je;&c_k;&c_o;</trd>,
      <trd>&c_v;&c_d;&c_a;&c_l;&c_i;</trd>
    </trdgrp>
  </drv>
  <drv mrk="for.0en">
    <kap><tld/>en</kap>
    <snc>
      <dif>
        Malproksimen:
        <ekz>
          ni rajdas multe da mejloj <tld/>en
          <fnt><bib>Fab2</bib>, <lok>Sambuka virineto</lok></fnt>;
        </ekz>
        <ekz>
          ili diskuris <tld/>en;
        </ekz>
        <ekz>
          voja&gcirc;i <tld/>en;
        </ekz>
        <ekz>
          tiun demandon li ju&gcirc;as de punkto tre alta kaj <tld/>en
          videbliganta.
        </ekz>
      </dif>
    </snc>
    <trd lng="de">weg</trd>
    <trdgrp lng="fr">
      <trd>loin</trd>,
      <trd>au <ind>loin</ind></trd>
    </trdgrp>
    <trd lng="nl">ver</trd>
    <trd lng="ru">&c_v;&c_d;&c_a;&c_l;&c_mol;</trd>
  </drv>
  <drv mrk="for.0ajxo">
    <kap><tld/>a&jcirc;o<fnt>K</fnt></kap>
    <snc>
      <dif>
        Malproksima&jcirc;o:
        <ekz>
          li malaperis en la <tld/>a&jcirc;o.
        </ekz>
      </dif>
    </snc>
    <trd lng="es">lejan&iacute;a</trd>
    <trdgrp lng="fr">
      <trd>lointain<klr tip="ind"> (subst.)</klr></trd>,
      <trd>horizon<klr tip="ind"> (le lointain)</klr></trd>,
      <trd>arri&egrave;re-plan</trd>
    </trdgrp>
    <trd lng="nl">verte</trd>
    <trd lng="ru">&c_d;&c_a;&c_l;&c_mol;</trd>
  </drv>
  <drv mrk="for.0igi">
    <kap><tld/>igi</kap>
    <snc>
      <dif>
        Malaperigi, neniigi:
        <ekz>
          <tld/>igi makulon, bre&ccirc;on, mankon;
        </ekz>
        <ekz>
          multaj &ccirc;agrenoj kaj suferoj povus esti <tld/>igataj,
          se ...<fnt>Z</fnt>;
        </ekz>
        <ekz>
          por <tld/>igi &ccirc;iujn
          dubojn<fnt>Z</fnt>, embarasojn<fnt>Z</fnt>
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="ekster1.0i">ekstermi</ref>,
        <ref cel="likvid.0i">likvidi</ref>,
        <ref cel="visx.for0i">forvi&scirc;i</ref>.
      </refgrp>
    </snc>
    <trdgrp lng="de">
      <trd>entfernen</trd>,
      <trd><ind>weg</ind> machen</trd>
    </trdgrp>
    <trd lng="es">alejar</trd>
    <trdgrp lng="fr">
      <trd>enlever</trd>,
      <trd>&eacute;loigner</trd>,
      <trd>&eacute;carter<klr tip="ind"> (&eacute;loigner)</klr></trd>,
      <trd>&ocirc;ter</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>verwijderen</trd>,
      <trd><ind>weg</ind> doen</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_u;&c_d;&c_a;&c_l;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_u;&c_b;&c_r;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_s;&c_t;&c_r;&c_a;&c_n;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_u;&c_s;&c_t;&c_r;&c_a;&c_n;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </drv>
  <drv mrk="for.0igxi">
    <kap><tld/>i&gcirc;i<fnt>Z</fnt></kap>
    <snc>
      <dif>
        Malaperi, nenii&gcirc;i:
        <ekz>
          iom post iom re<tld/>i&gcirc;is la akvo de sur la tero
          <fnt><bib>MT</bib>, <lok>&Gen; 8:3</lok></fnt>.
        </ekz>
      </dif>
    </snc>
    <trd lng="es">alejarse</trd>
    <trdgrp lng="fr">
      <trd>se <ind>retirer</ind></trd>,
      <trd>s'<ind>&eacute;loigner</ind></trd>
    </trdgrp>
    <trd lng="de">verschwinden</trd>
    <trdgrp lng="nl">
      <trd>verdwijnen</trd>,
      <trd><ind>weg</ind> gaan</trd>
    </trdgrp>
    <trd lng="ru">&c_u;&c_d;&c_a;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </drv>
  <drv mrk="for.de0a">
    <kap>de<tld/>a</kap>
    <snc mrk="for.de0a.TEK">
      <uzo tip="fak">TEK</uzo>
      <uzo tip="fak">KOMP</uzo>
      <dif>
        <ref tip="super" cel="for.0a.TEK">Fora</ref> kaj tia, ke
        la &ccirc;efaganto agas el loko alia ol la koncerna agadejo:
        <ekz>
          <uzo tip="fak">KOMP</uzo>
          <ind>de<tld/>a atingo</ind>
          <klr>(uzado de risurcoj de unu komputilo pere de alia
          komputilo, ekz-e per Telnet)</klr>;
          <trd lng="en">remote access</trd>
          <trd lng="ru">&c_u;&c_d;&c_a;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j; &c_d;&c_o;&c_s;&c_t;&c_u;&c_p;</trd>
        </ekz>
        <ekz>
          <uzo tip="fak">TEK</uzo>
          <ind>de<tld/>a stirado</ind>;
          <trd lng="de">Fernsteuerung</trd>
          <trd lng="en">remote control</trd>
          <trd lng="nl">afstandsbediening</trd>
          <trd lng="ru">&c_d;&c_i;&c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_o;&c_n;&c_n;&c_o;&c_je; &c_u;&c_p;&c_r;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>aus der <ind>Ferne</ind></trd>,
        <trd>von <ind>ferne</ind></trd>,
        <trd>von <ind>weitem</ind></trd>
        <trd>entfernt</trd>
      </trdgrp>
      <trd lng="en">remote</trd>
      <trd lng="nl">afstands-</trd>
      <trd lng="ru">&c_d;&c_i;&c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
    </snc>
  </drv>
  <drv mrk="for.antaux0igi">
    <kap>anta&ubreve;<tld/>igi</kap>
    <snc>
      <dif>
        Eviti, malebligi per anta&ubreve;a singardo:
        <ekz>
          anta&ubreve;<tld/>igi akcidenton.
        </ekz>
      </dif>
      <refgrp tip="vid">
        Preska&ubreve;aj sinonimoj:
        <ref cel="help.antauxmal0i">anta&ubreve;malhelpi</ref>,
        <ref cel="halt.antaux0igi">anta&ubreve;haltigi</ref>.
      </refgrp>
    </snc>
    <trd lng="de">vorsorglich <ind>verhindern</ind></trd>
    <trdgrp lng="fr">
      <trd>emp&ecirc;cher<klr tip="ind"> (pr&eacute;venir)</klr></trd>,
      <trd>pr&eacute;venir<klr tip="ind"> (&eacute;viter)</klr></trd>
    </trdgrp>
    <trd lng="nl">verhinderen</trd>
    <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_o;&c_t;&c_v;&c_r;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>
  </drv>
</subart>

<subart>
  <dif>
    Prefikso havanta la sencojn <sncref ref="for.0.malapero"/> kaj
    <sncref ref="for.0.malproksimeco"/> de la
    adverbo, sed plej ofte la unuan:
  </dif>
  <drv mrk="for.0akrigi">
    <kap><tld/>akrigi</kap>
    <snc>
      <dif>
        <tld lit="F"/>igi akrigante:
        <ekz>
          <tld/>akrigi la bre&ccirc;etojn<fnt>Z</fnt>.
        </ekz>
      </dif>
    </snc>
    <trd lng="ru">&c_s;&c_t;&c_o;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </drv>
  <drv mrk="for.0acxeti">
    <kap><tld/>a&ccirc;eti</kap>
    <snc>
      <dif>
        Malaperigi la varojn el la merkato a&ccirc;etante &ccirc;iom.
      </dif>
    </snc>
    <trd lng="fr">d&eacute;valiser<klr tip="ind"> (tout acheter)</klr></trd>
    <trd lng="de">aufkaufen</trd>
    <trdgrp lng="de">
       <trd>aufkaufen</trd>,
       <trd>leerkaufen</trd>,
       <trd>wegkaufen</trd>
    </trdgrp>
    <trd lng="nl">opkopen</trd>
    <trd lng="ru">&c_s;&c_k;&c_u;&c_p;&c_i;&c_t;&c_mol; <klr>(&c_v;&c_je;&c_s;&c_mol; &c_t;&c_o;&c_v;&c_a;&c_r;)</klr></trd>
  </drv>
  <drv mrk="for.0argumenti">
    <kap><tld/>argumenti</kap>
    <snc>
      <dif>
        Argumentante senpravigi kaj senvalorigi.
      </dif>
    </snc>
    <trd lng="de"><klr tip="ind">(jmd. etwas)</klr> ausreden</trd>
    <trd lng="fr">r&eacute;pliquer</trd>
    <trd lng="nl">uit het hoofd <ind>praten</ind></trd>
  </drv>
  <drv mrk="for.0beni">
    <kap><tld/>beni</kap>
    <snc>
      <dif>
        <tld lit="F"/>igi per beno:
        <ekz>
          <tld/>beni al iu la pekojn<fnt>Z</fnt>.
        </ekz>
      </dif>
    </snc>
    <ref cel="absolv.0i">absolvi</ref>
    <trd lng="fr">absoudre</trd>
    <trd lng="nl">vergeven</trd>
    <trd lng="ru">&c_o;&c_t;&c_p;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol;<klr tip="amb"> (&c_g;&c_r;&c_je;&c_h;&c_i;)</klr></trd>
  </drv>
  <drv mrk="for.0decidi">
    <kap><tld/>decidi<fnt>Z</fnt></kap>
    <snc>
      <dif>
        Decidi la <tld/>igon, <tld/>lason de io, rezigni pri io.
      </dif>
    </snc>
  </drv>
  <drv mrk="for.0danci0drinki0labori0ludi0dormi">
    <kap><tld/>danci, <tld/>drinki<fnt>Z</fnt>, <tld/>labori, <tld/>ludi,
    <tld/>dormi</kap>
    <snc>
      <dif>
        <tld lit="F"/>igi, perdi, forgesi, dancante, drinkante ktp:
        <ekz>
          <ind><tld/>drinki</ind> sian &ccirc;agrenon, sian monon;
          <trd lng="de">ers&auml;ufen</trd>
          <trd lng="fr">noyer<klr tip="amb"> (dans l'alcool)</klr></trd>
          <trd lng="nl">verdrinken<klr tip="ind"> (zijn zorgen
          bv.)</klr></trd>
          <trd lng="ru">&c_p;&c_r;&c_o;&c_p;&c_i;&c_t;&c_mol;</trd>
        </ekz>
        <ekz>
          <ind><tld/>dormi la okazon</ind> <fnt><bib>PrV</bib></fnt>;
          <trdgrp lng="de">
            <trd><klr>die </klr>Gelegenheit <ind>verschlafen</ind></trd>,
            <trd><klr>die </klr>Gelegenheit <ind>auslassen</ind></trd>,
            <trd><klr>die </klr>Sache <ind>verschlafen</ind></trd>
          </trdgrp>
          <trd lng="fr">manquer<klr tip="amb"> (une occasion)</klr></trd>
          <trd lng="nl">verspelen<klr tip="ind"> (kans bv.)</klr></trd>
          <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_p;&c_a;&c_t;&c_mol; &c_u;&c_d;&c_o;&c_b;&c_n;&c_y;&c_j; &c_s;&c_l;&c_u;&c_ch;&c_a;&c_j;</trd>          
        </ekz>
        <ekz>
          <tld/>dormi la tutan tagon.
        </ekz>
      </dif>
    </snc>
  </drv>
  <drv mrk="for.0difekti">
    <kap><tld/>difekti<fnt>Z</fnt></kap>
    <snc>
      <dif>
        Difekti objekton <tld/>igante ion de &gcirc;i.
      </dif>
    </snc>
  </drv>
  <drv mrk="for.0direkti">
    <kap><tld/>direkti</kap>
    <snc>
      <dif>
        Deturni:
        <ekz>
          <tld/>direkti sian &ccirc;aron<fnt>Z</fnt>.
        </ekz>
      </dif>
    </snc>
    <trd lng="fr">d&eacute;vier</trd>
    <trd lng="nl">doen <ind>afwijken</ind></trd>
  </drv>
  <drv mrk="for.0eskorti">
    <kap><tld/>eskorti</kap>
    <snc>
      <dif>
        <tld/>konduki eskortante.
      </dif>
    </snc>
    <trd lng="fr">reconduire<klr tip="ind"> (qn chez lui)</klr></trd>
    <trd lng="nl">begeleiden</trd>
    <trdgrp lng="ru">
      <trd>&c_u;&c_v;&c_je;&c_s;&c_t;&c_i;</trd>,
      <trd>&c_v;&c_y;&c_v;&c_je;&c_s;&c_t;&c_i;</trd>,
      <trd>&c_v;&c_y;&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </drv>
  <drv mrk="for.0frostigxi">
    <kap><tld/>frosti&gcirc;i</kap>
    <snc>
      <dif>
        Detrui&gcirc;i per frosto.
      </dif>
    </snc>
    <trd lng="fr">geler<klr tip="ind"> (&ecirc;tre abim&eacute; par le gel)</klr></trd>
    <trd lng="nl">bevriezen<klr tip="ind"> (door ijs
    beschadigen)</klr></trd>
  </drv>
  <drv mrk="for.0froti">
    <kap><tld/>froti</kap>
    <snc>
      <dif>
        <tld lit="F"/>igi frotante.
      </dif>
    </snc>
    <trd lng="fr">r&eacute;curer</trd>
    <trd lng="nl">wegkrabben</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_t;&c_je;&c_r;&c_je;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_t;&c_je;&c_r;&c_je;&c_t;&c_mol;</trd>
    </trdgrp>
  </drv>
  <drv mrk="for.0leki">
    <kap><tld/>leki</kap>
    <snc>
      <dif>
        <tld lit="F"/>igi lekante:
        <ekz>
          la fajro <tld/>lekis la akvon<fnt>Z</fnt>.
        </ekz>
      </dif>
    </snc>
    <trd lng="fr">tout <ind>nettoyer</ind><klr tip="amb"> (avec la
    langue)</klr></trd>
    <trd lng="nl">weglikken</trd>
    <trd lng="ru">&c_s;&c_l;&c_i;&c_z;&c_a;&c_t;&c_mol;</trd>
  </drv>
  <drv mrk="for.0malaperi">
    <kap><tld/>malaperi</kap>
    <snc>
      <dif>
        Tute malaperi.
      </dif>
    </snc>
    <trdgrp lng="de">
       <trd>verschwinden</trd>,
       <trd>sich in Luft aufl&ouml;sen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>s'<ind>&eacute;vaporer</ind><klr tip="ind"> (dispara&icirc;tre)</klr></trd>,
      <trd>se <ind>volatiliser</ind><klr tip="ind">
      (dispara&icirc;tre)</klr></trd>
    </trdgrp>
    <trd lng="nl">in <ind>rook</ind> opgaan</trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_p;&c_a;&c_s;&c_t;&c_mol;</trd>
  </drv>
  <drv mrk="for.0mangxi">
    <kap><tld/>man&gcirc;i</kap>
    <snc>
      <dif>
        Tute konsumi man&gcirc;ante.
      </dif>
    </snc>
    <trd lng="de">aufessen</trd>
    <trd lng="fr">tout <ind>boulotter</ind></trd>
    <trd lng="nl">opeten</trd>
    <trd lng="ru">&c_s;&c_malmol;&c_je;&c_s;&c_t;&c_mol;</trd>
  </drv>
  <drv mrk="for.0pafi">
    <kap><tld/>pafi</kap>
    <snc>
      <dif>
        <tld lit="F"/>igi pafante:
        <ekz>
          per sia mallerteco li nur <tld/>pafis la birdojn.
        </ekz>
      </dif>
       <trdgrp lng="de">
        <trd>abschie&szlig;en</trd>,
        <trd>umschie&szlig;en</trd>,
        <trd>verscheuchen</trd>
      </trdgrp>
      <trd lng="fr">effaroucher</trd>
      <trd lng="nl">wegschieten</trd>
      <trd lng="ru">&c_o;&c_t;&c_s;&c_t;&c_r;&c_je;&c_l;&c_i;&c_t;&c_mol;</trd>
    </snc>
  </drv>
  <drv mrk="for.0pafigxi">
    <kap><tld/>pafi&gcirc;i, sin <tld/>pafi, pafi sin <tld/></kap>
    <snc>
      <dif>
        Subite malaperi:
        <ekz>
          li pafis sin for<fnt><bib>PrV</bib></fnt>.
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="de">
        <trd>davonschie&szlig;en</trd>,
        <trd>davonsausen</trd>,
        <trd>davoneilen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>se tirer<klr tip="ind"> (s'en aller)</klr></trd>,
      <trd>filer<klr tip="ind"> (s'en aller)</klr></trd>
    </trdgrp>
    <trd lng="nl">wegschieten<klr tip="ind"> (snel weglopen)</klr></trd>
  </drv>
  <drv mrk="for.0preni">
    <kap><tld/>preni</kap>
    <snc>
      <dif>
        Preni por <tld/>igi.
      </dif>
    </snc>
    <trd lng="de">wegnehmen</trd>
    <trd lng="fr">emporter</trd>
    <trd lng="nl">wegnemen</trd>
    <trd lng="ru">&c_o;&c_t;&c_o;&c_b;&c_r;&c_a;&c_t;&c_mol;</trd>
  </drv>
  <drv mrk="for.0putri0putrigxi">
    <kap><tld/>putri<fnt>Z</fnt>, <tld/>putri&gcirc;i<fnt>Z</fnt></kap>
    <snc>
      <dif>
        Tute putri&gcirc;i, nenii&gcirc;i a&ubreve; senvalori&gcirc;i putri&gcirc;ante.
      </dif>
    </snc>
    <trd lng="de">verfaulen</trd>
    <trd lng="nl">wegrotten</trd>
    <trd lng="ru">&c_s;&c_g;&c_n;&c_i;&c_t;&c_mol;</trd>
  </drv>
  <drv mrk="for.0esti">
    <kap><tld/>esti</kap>
    <snc>
      <dif>
        Esti malproksime, ne &ccirc;eesti:
        <ekz>
          la <tld/>estanto &ccirc;iam estas malprava
          <fnt><bib>PrV</bib></fnt>
        </ekz>
      </dif>
    </snc>
    <trd lng="de">abwesend sein</trd>
    <trd lng="fr">&ecirc;tre <ind>absent</ind></trd>
    <trd lng="nl">afwezig zijn</trd>
    <trd lng="ru">&c_o;&c_t;&c_s;&c_u;&c_t;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </drv>
  <drv mrk="for.0kapti">
    <kap><tld/>kapti<fnt>Z</fnt></kap>
    <snc>
      <dif>
        Malproksimen konduki kiel kaptiton.
      </dif>
    </snc>
    <trd lng="de">entf&uuml;hren</trd>
    <trdgrp lng="fr">
      <trd>enlever<klr tip="ind"> (qn)</klr></trd>,
      <trd>ravir<klr tip="ind"> (enlever)</klr></trd>
    </trdgrp>
    <trd lng="nl">ontvoeren</trd>
  </drv>
  <drv mrk="for.0klini">
    <kap><tld/>klini</kap>
    <snc>
      <dif>
        Malproksimigi klinante, deturni:
        <ekz>
          <tld/>klini bran&ccirc;on;
        </ekz>
        <ekz>
          <uzo tip="stl">FIG</uzo>
          <tld/>klini la koleron de iu<fnt>Z</fnt>.
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="fr">
      <trd>d&eacute;tourner</trd>,
      <trd>&eacute;carter<klr tip="ind"> (d&eacute;tourner)</klr></trd>
    </trdgrp>
    <trd lng="nl">ombuigen</trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_t;&c_k;&c_l;&c_o;&c_n;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_k;&c_l;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </drv>
  <drv mrk="for.0meti">
    <kap><tld/>meti<fnt>Z</fnt></kap>
    <snc>
      <dif>
        Meti malproksimen, malproksimigi.
      </dif>
    </snc>
    <trd lng="fr">&ocirc;ter</trd>
    <trd lng="nl">wegzetten</trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_t;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_u;&c_b;&c_r;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </drv>
  <drv mrk="for.0ofendi">
    <kap><tld/>ofendi</kap>
    <snc>
      <dif>
        <tld lit="F"/>pu&scirc;i iun ofendante.
      </dif>
    </snc>
    <trd lng="fr">froisser<klr tip="ind"> (qn)</klr></trd>
  </drv>
  <drv mrk="for.0porti">
    <kap><tld/>porti<fnt>Z</fnt></kap>
    <snc>
      <dif>
        Porti malproksimen.
      </dif>
    </snc>
    <trdgrp lng="de">
      <trd>wegtragen</trd>,
      <trd>fortschaffen</trd>
    </trdgrp>
    <trd lng="fr">emporter</trd>
    <trd lng="nl">wegdragen</trd>
    <trd lng="ru">&c_u;&c_n;&c_je;&c_s;&c_t;&c_i;</trd>
  </drv>
  <drv mrk="for.0teni">
    <kap><tld/>teni</kap>
    <snc>
      <dif>
        Teni malproksimigante:
        <ekz>
          tio <tld/>tenas de Esperanto multajn
          personojn<fnt>Z</fnt>.
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="fr">
      <trd>tenir <ind>&eacute;loign&eacute;</ind></trd>,
      <trd>tenir <ind>&agrave; l'&eacute;cart</ind></trd>
    </trdgrp>
    <trd lng="de">abhalten</trd>
    <trd lng="nl">weghouden</trd>
  </drv>
  <drv mrk="for.0vagi">
    <kap><tld/>vagi</kap>
    <snc>
      <dif>
        Malproksimen vagi:
        <ekz>
          <tld/>vagas flanken &ccirc;iam liaj pensoj al la
          gepatra domo<fnt>Z</fnt>.
        </ekz>
      </dif>
    </snc>
    <trd lng="de">abschweifen</trd>
    <trdgrp lng="fr">
      <trd>divaguer</trd>,
      <trd>vagabonder</trd>
    </trdgrp>
    <trd lng="nl">wegzwerven</trd>
    <trd lng="ru">&c_u;&c_b;&c_r;&c_je;&c_s;&c_t;&c_i;</trd>
  </drv>
  <drv mrk="for.0veturi">
    <kap><tld/>veturi<fnt>Z</fnt></kap>
    <snc>
      <dif>
        <tld/>iri veturante.
      </dif>
    </snc>
    <trd lng="de">wegfahren</trd>
    <trd lng="fr">partir</trd>
    <trd lng="nl">wegrijden</trd>
    <trd lng="ru">&c_u;&c_je;&c_h;&c_a;&c_t;&c_mol;</trd>
  </drv>
