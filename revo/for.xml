<drv mrk="for.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      <ref cel="aper.mal0igi">Malaperigi</ref>, <ref cel="neni.0igi">neniigi</ref>:
      <ekz>
        <tld/>igu vian fraton, &ccirc;ar li malhelpas al ni
        <fnt>
          <bib>F</bib>
          <lok>&FE; 39</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igi eraron
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la egalrajtigo de &ccirc;iuj lingvoj kaj religioj <tld/>igos la ka&ubreve;zon de &ccirc;iuj militoj
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Dogmoj de Hilelismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igi la murojn inter la gentoj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Oka Kongreso Esperantista en Krakow en la 11a de a&ubreve;gusto 1912</vrk>
        </fnt>;
      </ekz>
      <ekz>
        tiuj &ccirc;i atakoj devigis min <tld/>igi mian ka&scirc;itecon
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Kion Zamenhof ne povis diri en &Gcirc;enevo.</vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>igis la rigardon de la tero kaj rapidigis siajn pa&scirc;ojn
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        per sia rideto li <tld/>igos la doloran rememoron
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igu lin de miaj okuloj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>!
      </ekz>
      <ekz>
        &scirc;i tiam <tld/>igis &ccirc;iujn interku&scirc;antajn malhelpojn
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi volis <tld/>igi &gcirc;in, <klr>[&#8230;]</klr> tiam mi fortran&ccirc;is la makulon kaj kovris
        la lokon
        <fnt>
          <bib>IdO</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        malbonon per malbono ni <tld/>igos
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld lit="F"/>igu mian dubon, ho, certigu, &parsl; Certigu al mi tute la feli&ccirc;on
        <fnt>
          <bib>Ifigenio</bib>
          <lok>Ifigenio en Ta&ubreve;rido</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiom da &ccirc;agreno kaj suferoj povus esti <tld/>igitaj, se &ccirc;iuj laboristoj vivus
        tiel prudente
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>! <tld/>igu lin
        <fnt>
          <bib>PrV</bib>
        </fnt>!
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekster1.0i">ekstermi</ref>,
      <ref cel="likvid.0i">likvidi</ref>,
      <ref cel="visx.for0i">forvi&scirc;i</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>odebrat</trd>,
    <trd>odklidit</trd>,
    <trd>odlo&zcaron;it</trd>,
    <trd>odstranit</trd>,
    <trd>vymazat</trd>,
    <trd>vyndat</trd>,
    <trd>vy&rcaron;adit</trd>,
    <trd>zahladit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>entfernen</trd>,
    <trd><ind>weg</ind> machen</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>suprimir</trd>,
    <trd>eliminar</trd>,
    <trd>quitar</trd>
  </trdgrp>
  <trdgrp lng="fi">
    <trd>poistaa</trd>,
    <trd>ottaa pois</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>enlever</trd>,
    <trd>&eacute;loigner</trd>,
    <trd>&eacute;carter<klr tip="ind"> (&eacute;loigner)</klr></trd>,
    <trd>&ocirc;ter</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verwijderen</trd>,
    <trd><ind>weg</ind> doen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_d;&c_a;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_b;&c_r;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_s;&c_t;&c_r;&c_a;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_s;&c_t;&c_r;&c_a;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">odstr&aacute;ni&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_b;&c_i;&c_r;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_u;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_d;&c_a;&c_l;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_s;&c_t;&c_o;&c_r;&c_o;&c_n;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_n;&c_i;&c_shch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="for.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      <ref cel="aper.mal0i">Malaperi</ref>, nenii&gcirc;i:
      <ekz>
        iom post iom re<tld/>i&gcirc;is la akvo de sur la tero
        <fnt><bib>MT</bib>, <lok>&Gen; 8:3</lok></fnt>.
      </ekz>
      <ekz>
        la &scirc;ipo <tld/>i&gcirc;is iom post iom al la maro
        <fnt>
          <bib>DKM</bib>
          <lok>&Ccirc;apitro LXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia &ccirc;agreno <tld/>i&gcirc;is, &ccirc;ar mi povus refoje promeni kaj ludi
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        Mario <tld/>i&gcirc;is en la profundon de la salono
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj fortoj <tld/>i&gcirc;is <klr>[&#8230;]</klr> kaj du jarojn post la morto de Kristoforo
        &scirc;i silente endormi&gcirc;is
        <fnt>
          <bib>HsT</bib>
          <lok>Heredanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        de plendo kaj ploro ne <tld/>i&gcirc;as doloro
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">alejarse</trd>
  <trdgrp lng="fi">
    <trd>poistua</trd>,
    <trd>h&auml;vit&auml;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>retirer</ind></trd>,
    <trd>s'<ind>&eacute;loigner</ind></trd>
  </trdgrp>
  <trd lng="de">verschwinden</trd>
  <trdgrp lng="nl">
    <trd>verdwijnen</trd>,
    <trd><ind>weg</ind> gaan</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_d;&c_a;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_d;&c_a;&c_l;&c_ja;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_b;&c_i;&c_r;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_u;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

