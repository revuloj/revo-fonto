<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: for.xml,v 1.76 2025/12/13 02:12:21 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>for</rad>
  <fnt> <bib>UV</bib></fnt>
</kap>
<subart mrk="for.0.adverbo">
<drv mrk="for.0">
  <kap><tld/></kap>
  <dif>
    Adverbo signifanta:
  </dif>
  <snc mrk="for.0.malapero">
    <dif>
      Malaperon kaj perdi&gcirc;on en malproksimo:
      <ekz>
        iru <tld/>
        <fnt>
          <bib>F</bib>
          <lok>&FE; 20</lok>
        </fnt>!
      </ekz>
      <ekz>
        <tld/> de tie &ccirc;i
        <fnt>
          <bib>F</bib>
          <lok>&FE; 26</lok>
        </fnt>!
      </ekz>
      <ekz>
        dormo kuris <tld/> de miaj okuloj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 31:40</lok>
        </fnt>;
      </ekz>
      <ekz>
        lasu min <tld/>
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 24:54</lok>
        </fnt>;
      </ekz>
      <ekz>
        li lasis sian veston &ccirc;e mi kaj <tld/>kuris <tld/> el la domo
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 39:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/> de miaj okuloj, mi ne volas plu vidi vin
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fabelo pri iu, kiu migris por ekkoni timon</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam <klr>[&#8230;]</klr> ili estas <tld/>, li elrampis
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i levadis la okulojn <tld/> de la laboro
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/> kun &gcirc;i
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>!
      </ekz>
      <ekz>
        <tld/>, <tld/>! bojis la hundo
        <fnt>
          <bib>Fab4</bib>
          <lok>Ne&gcirc;ulo</lok>
        </fnt>;
      </ekz>
      <ekz>
        nun &ccirc;iu espero estas <tld/>
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Batalo je la Ebura Turo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/> de l' okuloj, <tld/> de la koro
        <fnt><bib>PrV</bib></fnt>;
      </ekz><ekz>
        pasinta doloro <tld/> el memoro
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="for.0en"><tld/>en</ref>
    <trdgrp lng="de">
      <trd>weg</trd>,
      <trd>fort</trd>
    </trdgrp>
    <trd lng="en">away</trd>
    <trd lng="es">fuera</trd>
    <trdgrp lng="fi">
      <trd>pois</trd>,
      <trd>poissa</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>longe</trd>,
      <trd>fora</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_ch;&c_mol;</trd>,
      <trd>&c_v;&c_o;&c_n;</trd>,
      <trd>&c_d;&c_o;&c_l;&c_o;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="for.0.malproksimeco">
    <dif>
      Malproksimecon:
      <ekz>
        <nom>Jehuda</nom> aparti&gcirc;is <tld/> de siaj fratoj,
        kaj eklo&gcirc;is apud iu Adulamano
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 38:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Marta</nom> staris jam <ind>kelke da pa&scirc;oj <tld/> de
        &scirc;i</ind>
        <fnt>
          <bib>Marta</bib>
        </fnt>;
        <trd lng="fi">muutaman askelen <ind>p&auml;&auml;ss&auml;</ind>
           h&auml;nest&auml;</trd>
      </ekz>
      <ekz>
        &ccirc;u ci kredas, ke min &gcirc;enus esti <tld/> de la le&gcirc;eco
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>?
      </ekz>
      <ekz>
        ve al tiu, kiu devas vivi <tld/> de la gepatroj kaj de la
        gefratoj
        <fnt>
          <bib>Ifigenio</bib>
          <lok>Ifigenio en Ta&ubreve;rido</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la kampo, <tld/> de l' mondo
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bankiero posedas kampodomon post unu kilometro <tld/> de la stacidomo
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        preska&ubreve; 300 kilometrojn <tld/> de &ccirc;i tie
        <fnt>
          <bib>ChV</bib>
          <lok>9</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ejo estis tiom <tld/> de la urbocentro
        <fnt>
          <bib>Kon11</bib>
          <lok>La IJK estas la homoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        li sendis ilin <tld/> en la mondon, &ccirc;iun en apartan malproksiman landon
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="for.0e"><tld/>e</ref>
    <ref tip="vid" cel="proksi.mal0ede">malproksime de</ref>
    <trdgrp lng="de">
      <trd>entfernt</trd>,
      <trd>weit</trd>
    </trdgrp>
    <trd lng="en">far</trd>
    <trd lng="es">lejos</trd>
    <trdgrp lng="fi">
      <trd>kaukana</trd>,
      <trd>et&auml;&auml;ll&auml;</trd>,
      <trd>kauas</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>longe</trd>,
      <trd>fora</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_d;&c_a;&c_l;&c_je;&c_k;&c_je;<klr> (&c_o;&c_t; &c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>,
      <trd>&c_v;&c_d;&c_a;&c_l;&c_i;<klr> (&c_o;&c_t; &c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>,
      <trd>&c_d;&c_a;&c_l;&c_je;&c_k;&c_o;<klr> (&c_o;&c_t; &c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="for.0.malcxi">
    <uzo tip="stl">RAR</uzo>
    <dif>
      <ref tip="dif" cel="cxi.mal0">Mal&ccirc;i</ref>:
      <ekz>
        du &ccirc;evaloj, tiu &ccirc;i kaj tiu <tld/>
        <fnt><bib>PAG</bib>, <lok>&para;54, Rim.V</lok></fnt>;
      </ekz>
      <ekz>
        ku&scirc;as vestoj de viro tie <tld/>, iu viro banas sin tute apude
        <fnt>
          <aut>Hjalmar S&ouml;derberg, tr. Sten Johansson</aut>:
          <vrk>La bovinoj de la paro&hcirc;estro</vrk>.
          <lok>La Kiso Kaj dek tri aliaj noveloj, 1955</lok>
          </fnt>.
      </ekz>
    </dif>
    <rim>
      Ne konfuzu &ccirc;i tian <tld/><sncref ref="for.0.malcxi"/>
      kun <tld/><sncref ref="for.0.malapero"/>:
      <ekz>
        forpelu de mi &ccirc;i tiun <tld/><sncref ref="for.0.malapero"/>,
        kaj &scirc;losu la pordon post &scirc;i!
        <fnt><bib>MT</bib>, <lok>2Sam 13:17</lok></fnt>
      </ekz>
    </rim>
  <trd lng="de">dort</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_v;&c_o;&c_d;&c_d;&c_a;&c_l;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_l;&c_je;&c_j;</trd>,
    <trd>&c_n;&c_ja; &c_t;&c_u;&c_t;</trd>,
    <trd>&c_u; <ind>&c_d;&c_a;&c_l;&c_je;&c_ch;&c_y;&c_n;&c_ib;</ind></trd>
  </trdgrp>
  <trd lng="bo">&t_pha;&t_ra;&t_tsheg;</trd>
  <trd lng="cs">pry&ccaron;</trd>
  <trd lng="fr">loin</trd>
  <trd lng="ja">&#21435;&#12428;&#65281;  <pr>&#12373;&#12428;&#65281;</pr></trd>
  <trdgrp lng="nl">
    <trd>weg</trd>,
    <trd>uit</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>z <ind>dala</ind></trd>,
    <trd>poza</trd>,
    <trd>dalej</trd>
  </trdgrp>
  <trd lng="sk">pre&ccaron;</trd>
  <trd lng="sw">mbali</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_l;&c_je;&c_k;&c_o;</trd>
  </trdgrp>
</drv>
</subart>

<subart mrk="for.0.vortero">
  <dif>
    Vortero signifanta malproksimecon a&ubreve; malaperon (kaj eble
    nenii&gcirc;on):
  </dif>

<drv mrk="for.0a">
  <kap><tld/>a</kap>
  <snc mrk="for.0a.KOMUNE">
    <dif>
      <ref tip="sin" cel="proksi.mal0a">Malproksima</ref>:
      <ekz>
        la blankaj ne&gcirc;ovastoj etendis sin &gcirc;is la <tld/>a horizonto
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        bela reva&jcirc;o kaj tamen pli reala ol &ccirc;io alia, nun pali&gcirc;inta kaj <tld/>a
        <fnt>
          <bib>HsT</bib>
          <lok>La nepo</lok>
        </fnt>;
      </ekz>
      <ekz>
        en <tld/>a estonteco
        <fnt>
          <bib>EeP</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la religiaj bataloj de tiu <tld/>a epoko
        <fnt>
          <bib>KKr</bib>
          <lok>II</lok>
        </fnt>;
      </ekz>
      <ekz>
        lo&gcirc;antoj de <tld/>a planedo
        <fnt>
          <bib>ChV</bib>
          <lok>7</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u ni iru al iom pli <tld/>a loko
        <fnt>
          <bib>FSI</bib>
          <lok>IV</lok>
        </fnt>?
      </ekz>
      <ekz>
        mi preska&ubreve; nenion memoras pri tia <tld/>eco
        <fnt>
          <bib>Monato</bib>
          <aut>Petro de Smedt</aut>
          <vrk><url ref="&Monato;/2010/009946.php">La verde farbita birdo</url></vrk>
          <lok>2010</lok>
        </fnt>!
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="proksi.mal0a">malproksima</ref>,
      <ref cel="lontan.0a">lontana</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>entfernt</trd>,
      <trd>abgelegen</trd>,
      <trd>weit weg</trd>,
      <trd>weit entfernt</trd>
    </trdgrp>
    <trd lng="es">lejano</trd>
    <trdgrp lng="fi">
      <trd>kaukainen</trd>,
      <trd>et&auml;inen</trd>
    </trdgrp>
    <trd lng="nl">afgelegen</trd>
    <trdgrp lng="zh">
    <trd>&#32236; <pr>mi&#462;n</pr></trd>,
    <trd>&#36828;&#38548; <pr>yu&#462;ng&eacute;</pr></trd>,
    <trd>&#28218; <pr>mi&#462;o</pr></trd>,
    <trd>&#36916; <pr>chu&ograve;</pr></trd>,
    <trd>&#36944; <pr>xi&aacute;</pr></trd>,
    <trd>&#28218;&#33579; <pr>mi&#462;om&aacute;ng</pr></trd>,
    <trd>&#36797; <pr>li&aacute;o</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="for.0a.TEK">
    <uzo tip="fak">TEK</uzo>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Pri komputilo a&ubreve; aran&gcirc;a&jcirc;o atingata per komunika lineo:
      <ekz>
        <ind><tld/>a terminalo</ind>;
        <trd lng="de">Remoteterminal</trd>
        <trd lng="en">remote terminal</trd>
        <trd lng="pl">terminal zdalny</trd>
        <trd lng="ru">&c_u;&c_d;&c_a;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j; &c_t;&c_je;&c_r;&c_m;&c_i;&c_n;&c_a;&c_l;</trd>
      </ekz><ekz>
        <ind><tld/>a uzanto</ind>;
        <trd lng="de">Remotebenutzer</trd>
        <trd lng="en">remote user</trd>
        <trd lng="es">usuario remoto</trd>
        <trd lng="pl">u&zdot;ytkownik zdalny</trd>
        <trd lng="ru">&c_d;&c_i;&c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j; &c_p;&c_o;&c_l;&c_mol;&c_z;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>entfernt</trd>,
      <trd>remote</trd>
    </trdgrp>
    <trd lng="en">remote</trd>
    <trd lng="es">remoto</trd>
    <trd lng="fi">et&auml;-</trd>
    <trd lng="ru">&c_d;&c_i;&c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_l;&c_jo;&c_k;&c_ib;</trd>,
    <trd>&c_a;&c_d;&c_d;&c_a;&c_l;&c_je;&c_n;&c_y;</trd>,
    <trd>&c_d;&c_y;&c_s;&c_t;&c_a;&c_n;&c_c;&c_y;&c_j;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bo">&t_tha;&t_ga;&t_tsheg;&t_ra;&t_gata;&t_yata;&t_nga;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>d&aacute;lkov&yacute;</trd>,
    <trd>na d&aacute;lku</trd>,
    <trd>vzd&aacute;len&yacute;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>lointain<klr tip="ind"> (adj.)</klr></trd>,
    <trd>&eacute;loign&eacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12399;&#12427;&#12363;&#12394;</trd>,
    <trd>&#36960;&#12367;&#38626;&#12428;&#12383;
    <pr>&#12392;&#12362;&#12367;&#12399;&#12394;&#12428;&#12383;</pr></trd>
  </trdgrp>
  <trd lng="nl">afstands-</trd>
  <trdgrp lng="pl">
    <trd>oddalony</trd>,
    <trd>daleki</trd>
  </trdgrp>
  <trd lng="ro">departe</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_a;&c_l;&c_jo;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_u;&c_d;&c_a;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_t;&c_d;&c_a;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>d&aacute;vny</trd>,
    <trd>vzdialen&yacute;</trd>
  </trdgrp>
  <trd lng="sw">a mbali (kivumishi)</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_a;&c_l;&c_je;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_d;&c_a;&c_l;&c_je;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="for.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Malproksime (senambigua formo de
      <ref tip="sin" cel="for.0.malproksimeco"><tld/><sncref/></ref>):
      <ekz>
        <tld/>e de la homoj
        <fnt>
          <bib>HsT</bib>
          <lok>La nepo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li sidis senmove kaj <tld/>e de &ccirc;io proksima
        <fnt>
          <bib>HsT</bib>
          <lok>La nepo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e, supre, vidi&gcirc;as kruco
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XLVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        li time rerigardis: &scirc;i jam estis <tld/>e
        <fnt>
          <bib>SrT</bib>
          <lok>Libro I.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Vojski</nom> al gasto brakojn etendis de <tld/>e
        <fnt>
          <bib>SrT</bib>
          <lok>Libro I.</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_l;&c_jo;&c_k;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_o;&c_d;&c_d;&c_a;&c_l;&c_mol;</trd>,
    <trd>&c_u; <ind>&c_d;&c_a;&c_l;&c_je;&c_ch;&c_y;&c_n;&c_ib;</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>daleko</trd>,
    <trd>pry&ccaron;<klr> (kde?)</klr></trd>,
    <trd>v d&aacute;lce</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>abgelegen</trd>,
    <trd>j.w.d.</trd>,
    <trd>jwd</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>lejanamente</trd>
  </trdgrp>
  <trdgrp lng="fi">
    <trd>kaukana</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>loin</trd>,
    <trd>au <ind>loin</ind></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12399;&#12427;&#12363;&#12395;</trd>,
    <trd>&#36960;&#12367;&#12395;  <pr>&#12392;&#12362;&#12367;&#12395;</pr></trd>
  </trdgrp>
  <trd lng="nl">ver</trd>
  <trdgrp lng="pl">
    <trd>z <ind>daleka</ind></trd>,
    <trd>daleko</trd>,
    <trd>w <ind>oddali</ind></trd>,
    <trd>aut</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_a;&c_l;&c_je;&c_k;&c_o;</trd>,
    <trd>&c_v;&c_d;&c_a;&c_l;&c_i;</trd>
  </trdgrp>
  <trd lng="sk">v dia&lcaron;ke</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_a;&c_l;&c_je;&c_k;&c_o;</trd>,
    <trd>&c_u;&c_d;&c_a;&c_l;&c_i;&c_n;&c_ib;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_d;&c_a;&c_l;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#36828;&#31163; <pr>yu&#462;nl&iacute;</pr></trd>,
    <trd>&#20731; <pr>p&igrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="for.0en">
  <kap><tld/>en</kap>
  <snc>
    <dif>
      Malproksimen (senambigua formo de
      <ref tip="sin" cel="for.0.malapero"><tld/><sncref/></ref>):
      <ekz>
        ni rajdas multe da mejloj <tld/>en
        <fnt><bib>Fab2</bib>, <lok>Sambuka virineto</lok></fnt>;
