<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>for</rad></kap>
<subart>
  <dif>
    Adv. signifanta: 
  </dif>
  <snc mrk="for.0.malapero">
    <dif>
      Malaperon kaj perdi&gcirc;on en malproksimo:
      <ekz>
        <tld/> de l' okuloj, <tld/> de la koro
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
      </ekz>
      <ekz>
        pasinta doloro <tld/> el memoro
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
      </ekz>
      <ekz>
        <tld/> vi(n)!
      </ekz>
      <ekz>
        <tld/> la manoj(n)!
      </ekz>
      <ekz>
        <tld/> la revoj(n)!
      </ekz>
      <ekz>
        <tld/> plezuro(n)!
      </ekz>
      <ekz>
        &ccirc;iu espero estas <tld/>;
      </ekz>
      <ekz>
        la digo estis <tld/> kaj la torento povis libere elfluegi.
      </ekz>
    </dif>
  </snc>
  <snc mrk="for.0.malproksimeco">
    <dif>
      Malproksimecon:
      <ekz>
        Marta staris jam kelke da pa&scirc;oj <tld/> de
        &scirc;i<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ve al tiu, kiu devas vivi <tld/> de la gepatroj kaj de la
        gefratoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        sur la kampo, <tld/> de l' mondo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        fikse rigardantaj <tld/> anta&ubreve;en sur la nigra oceano;
      </ekz>
      <ekz>
        la libretoj estis dissenditaj <tld/> en la mondon;
      </ekz>
      <ekz>
        tio estas <tld/> de mia intenco.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>weg</trd>, <trd>fort</trd>, <trd>entfernt</trd>, <trd>weit</trd>
  </trdgrp>
  <trd lng="fr">loin</trd>
</subart>
<subart>
  <dif>
    Vortero signifanta malproksimecon a&ubreve; malaperon k nenii&gcirc;on: 
  </dif>
  <drv mrk="for.0a">
    <kap><tld/>a</kap>
    <snc>
      <dif>
        Malproksima:
        <ekz>
          li lo&gcirc;as en <tld/>a angulo de Siberio;
        </ekz>
        <ekz>
          nia plej <tld/>a kolonio;
        </ekz>
        <ekz>
          la <tld/>a horizonto;
        </ekz>
        <ekz>
          <tld/>aj veloj glitis sur la maro;
        </ekz>
        <ekz>
          <tld/>a estonteco;
        </ekz>
        <ekz>
          en seskilometra <tld/>eco trovi&gcirc;as ruinoj.
        </ekz>
      </dif>
    </snc>
    <trd lng="cs">vzd&aacute;len&yacute;</trd>
    <trdgrp lng="fr">
      <trd>lointain <klr tip="ind">(adj.)</klr></trd>,
      <trd>&eacute;loign&eacute;</trd></trdgrp> 
  </drv>
  <drv mrk="for.0e">
    <kap><tld/>e</kap>
    <snc>
      <dif>
        Malproksime:
        <ekz>
          mi lo&gcirc;as pli <tld/>e;
        </ekz>
        <ekz>
          la stacidomo estas <tld/>e je duonhoro;
        </ekz>
        <ekz>
          li etendis al la amiko la brakojn de <tld/>e.
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="fr">
      <trd>loin</trd>,
      <trd>au <ind>loin</ind></trd></trdgrp> 
  </drv>
  <drv mrk="for.0en">
    <kap><tld/>en</kap>
    <snc>
      <dif>
        Malproksimen:
        <ekz>
          ni rajdas multe da mejloj <tld/>en
          <fnt>(<aut>Andersen, trad. Zamenhof</aut>: <vrk>Fabeloj,
          Sambuka virineto</vrk>)</fnt>;
        </ekz>
        <ekz>
          ili diskuris <tld/>en;
        </ekz>
        <ekz>
          voja&gcirc;i <tld/>en;
        </ekz>
        <ekz>
          tiun demandon li ju&gcirc;as de punkto tre alta k <tld/>en
          videbliganta.
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="fr">
      <trd>loin</trd>,
      <trd>au <ind>loin</ind></trd></trdgrp> 
  </drv>
  <drv mrk="for.0ajxo">
    <kap><tld/>a&jcirc;o<fnt>K</fnt></kap>
    <snc>
      <dif>
        Malproksima&jcirc;o:
        <ekz>
          li malaperis en la <tld/>a&jcirc;o.
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="fr">
      <trd>lointain <klr tip="ind">(subst.)</klr></trd>,
      <trd>horizon <klr>(le lointain)</klr></trd>,
      <trd>arri&egrave;re-plan</trd></trdgrp> 
  </drv>
  <drv mrk="for.0igi">
    <kap><tld/>igi</kap>
    <snc>
      <dif>
        Malaperigi, neniigi:
        <ekz>
          <tld/>igi makulon, bre&ccirc;on, mankon;
        </ekz>
        <ekz>
          multaj &ccirc;agrenoj kaj suferoj povus esti <tld/>igataj,
          se...<fnt>Z</fnt>;
        </ekz>
        <ekz>
          por <tld/>igi &ccirc;iujn
          dubojn<fnt>Z</fnt>, embarasojn<fnt>Z</fnt>
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="ekster1.0i">ekstermi</ref>,
        <ref cel="likvid.0i">likvidi</ref>,
        <ref cel="visx.for0i">forvi&scirc;i</ref>.
      </refgrp>
    </snc>
    <trdgrp lng="de">
      <trd>entfernen</trd>,
      <trd><ind>weg</ind> machen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>enlever</trd>,
      <trd>&eacute;loigner</trd>,
      <trd>&eacute;carter <klr tip="ind">(&eacute;loigner)</klr></trd>,
      <trd>&ocirc;ter</trd></trdgrp> 
  </drv>
  <drv mrk="for.0igxi">
    <kap><tld/>i&gcirc;i<fnt>Z</fnt></kap>
    <snc>
      <dif>
        Malaperi, nenii&gcirc;i:
        <ekz>
          iom post iom re<tld/>i&gcirc;is la akvo de sur la tero
          <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
          <lok>Genezo 8, 3</lok>)</fnt>.
        </ekz>
      </dif> 
    </snc>
    <trdgrp lng="fr">
      <trd>se <ind>retirer</ind></trd>,
      <trd>s'<ind>&eacute;loigner</ind></trd></trdgrp> 
    <trd lng="de">verschwinden</trd>
  </drv>
</subart>
<subart>
  <dif>
    Prefikso havanta la sencojn <sncref ref="for.0.malapero"/> kaj
    <sncref ref="for.0.malproksimeco"/> de la
    adverbo, sed plej ofte la unuan:
  </dif>
  <drv mrk="for.0akrigi">
    <kap><tld/>akrigi</kap>
    <snc>
      <dif>
        <tld lit="F"/>igi akrigante:
        <ekz>
          <tld/>akrigi la bre&ccirc;etojn<fnt>Z</fnt>.
        </ekz> 
      </dif>
    </snc>
  </drv>
  <drv mrk="for.0acxeti">
    <kap><tld/>a&ccirc;eti</kap>
    <snc>
      <dif>
        Malaperigi la varojn el la merkato a&ccirc;etante &ccirc;iom. 
      </dif>
    </snc>
    <trd lng="fr">d&eacute;valiser <klr tip="ind">(tout acheter)</klr></trd>
    <trd lng="de">aufkaufen</trd>
  </drv>
  <drv mrk="for.0argumenti">
    <kap><tld/>argumenti</kap>
    <snc>
      <dif>
        Argumentante senpravigi kaj senvalorigi. 
      </dif>
    </snc>
    <trd lng="de"><klr tip="ind">(jmd. etwas)</klr> ausreden</trd>
    <trd lng="fr">r&eacute;pliquer</trd>
  </drv>
  <drv mrk="for.0beni">
    <kap><tld/>beni</kap>
    <snc>
      <dif>
        <tld lit="F"/>igi per beno:
        <ekz>
          <tld/>beni al iu la pekojn<fnt>Z</fnt>.
        </ekz> 
      </dif>
    </snc>
    <ref cel="absolv.0i">absolvi.</ref>
    <trd lng="fr">absoudre</trd>
  </drv>
  <drv mrk="for.0decidi">
    <kap><tld/>decidi<fnt>Z</fnt></kap>
    <snc>
      <dif>
        Decidi la <tld/>igon, <tld/>lason de io, rezigni pri io.
      </dif>
    </snc>
  </drv>
  <drv mrk="for.0danci0drinki0labori0ludi0dormi">
    <kap><tld/>danci, <tld/>drinki<fnt>Z</fnt>
    <tld/>labori, <tld/>ludi, <tld/>dormi</kap>
    <snc>
      <dif>
        <tld lit="F"/>igi, perdi, forgesi, dancante, drinkante ktp:
        <ekz>
          <ind><tld/>drinki</ind> sian &ccirc;agrenon, sian monon;
          <trd lng="fr">noyer <klr>(dans l'alcool)</klr></trd>
        </ekz>
        <ekz>
          <ind><tld/>dormi la okazon</ind>
          <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
          <trd lng="fr">manquer <klr tip="amb">(une occasion)</klr></trd>
        </ekz>
        <ekz>
          <tld/>dormi la tutan tagon.
        </ekz>
      </dif>
    </snc>
  </drv>
  <drv mrk="for.0difekti">
    <kap><tld/>difekti<fnt>Z</fnt></kap>
    <snc>
      <dif>
        Difekti objekton <tld/>igante ion de &gcirc;i. 
      </dif>
    </snc>
  </drv>
  <drv mrk="for.0direkti">
    <kap><tld/>direkti</kap>
    <snc>
      <dif>
        Deturni:
        <ekz>
          <tld/>direkti sian &ccirc;aron<fnt>Z</fnt>.
        </ekz> 
      </dif>
    </snc>
    <trd lng="fr">d&eacute;vier</trd>
  </drv>
  <drv mrk="for.0eskorti">
    <kap><tld/>eskorti</kap>
    <snc>
      <dif>
        <tld/>konduki eskortante.  
      </dif>
    </snc>
    <trd lng="fr">reconduire <klr tip="ind">(qn chez lui)</klr></trd>
  </drv>
  <drv mrk="for.0frostigxi">
    <kap><tld/>frosti&gcirc;i</kap>
    <snc>
      <dif>
        Detrui&gcirc;i per frosto. 
      </dif>
    </snc>
    <trd lng="fr">geler <klr tip="ind">(&ecirc;tre abim&eacute; par le gel)</klr></trd>
  </drv>
  <drv mrk="for.0froti">
    <kap><tld/>froti</kap>
    <snc>
      <dif>
        <tld lit="F"/>igi frotante. 
      </dif>
    </snc>
    <trd lng="fr">r&eacute;curer</trd>
  </drv>
  <drv mrk="for.0leki">
    <kap><tld/>leki</kap>
    <snc>
      <dif>
        <tld lit="F"/>igi lekante:
        <ekz>
          la fajro <tld/>lekis la akvon<fnt>Z</fnt>.
       </ekz> 
     </dif>
   </snc>
  <trd lng="fr">tout <ind>nettoyer</ind> <klr>(avec la langue)</klr></trd>
</drv>
<drv mrk="for.0malaperi">
  <kap><tld/>malaperi</kap>
  <snc>
    <dif>
      Tute malaperi. 
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>s'<ind>&eacute;vaporer</ind> <klr tip="ind">(dispara&icirc;tre)</klr></trd>,
    <trd>se <ind>volatiliser</ind> <klr tip="ind">(dispara&icirc;tre)</klr></trd></trdgrp> 
</drv>
<drv mrk="for.0mangxi">
  <kap><tld/>man&gcirc;i</kap>
  <snc>
    <dif>
      Tute konsumi man&gcirc;ante. 
    </dif>
  </snc>
  <trd lng="de">aufessen</trd>
  <trd lng="fr">tout <ind>boulotter</ind></trd>
</drv>
<drv mrk="for.0pafi">
  <kap><tld/>pafi</kap>
  <snc>
    <dif>
      <tld lit="F"/>igi pafante:
      <ekz>
        per sia mallerteco li nur <tld/>pafis la birdojn.
      </ekz>
    </dif>
    <trd lng="fr">effaroucher</trd>
  </snc>
</drv>
<drv mrk="for.0pafigxi">
  <kap><tld/>pafi&gcirc;i, sin <tld/>pafi, pafi sin <tld/></kap>
  <snc>
    <dif>
      Subite malaperi:
      <ekz>
        li pafis sin for
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>.
      </ekz> 
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>se tirer <klr tip="ind">(s'en aller)</klr></trd>,
    <trd>filer <klr tip="ind">(s'en aller)</klr></trd></trdgrp> 
</drv>
<drv mrk="for.0preni">
  <kap><tld/>preni</kap>
  <snc>
    <dif>
      Preni por <tld/>igi.
    </dif>
  </snc>
  <trd lng="de">wegnehmen</trd>
  <trd lng="fr">emporter</trd>
</drv>
<drv mrk="for.0putri0putrigxi">
  <kap><tld/>putri<fnt>Z</fnt>, <tld/>putri&gcirc;i<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Tute putri&gcirc;i, nenii&gcirc;i  a&ubreve; senvalori&gcirc;i
      putri&gcirc;ante.
    </dif>
  </snc>
  <trd lng="de">verfaulen</trd>
</drv>
<drv mrk="for.antaux0igi">
  <kap>anta&ubreve;<tld/>igi</kap>
  <snc>
    <dif>
      Eviti, malebligi per anta&ubreve;a singardo:
      <ekz>
        anta&ubreve;<tld/>igi akcidenton.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">pr&eacute;venir <klr tip="ind">(&eacute;viter)</klr></trd>
</drv>
<drv mrk="for.0esti">
  <kap><tld/>esti</kap>
  <snc>
    <dif>
      Esti malproksime, ne &ccirc;eesti:
      <ekz>
        <tld/>estanto &ccirc;iam estas malprava
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>.
      </ekz> 
    </dif>
  </snc>
  <trd lng="de"><ind>abwesend</ind> sein</trd>
  <trd lng="fr">&ecirc;tre <ind>absent</ind></trd>
</drv>
<drv mrk="for.0kapti">
  <kap><tld/>kapti<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Malproksimen konduki kiel kaptiton. 
    </dif>
  </snc>
  <trd lng="de">entf&uuml;hren</trd>
  <trdgrp lng="fr">
    <trd>enlever <klr tip="ind">(qn)</klr></trd>,
    <trd>ravir <klr tip="ind">(enlever)</klr></trd></trdgrp> 
</drv>
<drv mrk="for.0klini">
  <kap><tld/>klini</kap>
  <snc>
    <dif>
      Malproksimigi klinante, deturni:
      <ekz>
        <tld/>klini bran&ccirc;on <klr>(f)</klr>;
      </ekz>
      <ekz>
        <tld/>klini la koleron de iu<fnt>Z</fnt>.
      </ekz> 
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>d&eacute;tourner</trd>,
    <trd>&eacute;carter <klr tip="ind">(d&eacute;tourner)</klr></trd></trdgrp> 
</drv>
<drv mrk="for.0meti">
  <kap><tld/>meti<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Meti malproksimen, malproksimigi.  
    </dif>
  </snc>
  <trd lng="fr">&ocirc;ter</trd>
</drv>
<drv mrk="for.0ofendi">
  <kap><tld/>ofendi</kap>
  <snc>
    <dif>
      <tld/>pu&scirc;i iun ofendante. 
    </dif>
  </snc>
  <trd lng="fr">froisser <klr tip="ind">(qn)</klr></trd>
</drv>
<drv mrk="for.0porti">
  <kap><tld/>porti<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Porti malproksimen. 
    </dif>
  </snc>
  <trd lng="fr">emporter</trd>
</drv>
<drv mrk="for.0teni">
  <kap><tld/>teni</kap>
  <snc>
    <dif>
      Teni malproksimigante:
      <ekz>
        tio <tld/>tenas de esp-o multajn
        personojn<fnt>Z</fnt>.
      </ekz> 
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>tenir <ind>&eacute;loign&eacute;</ind></trd>,
    <trd>tenir <ind>&agrave; l'&eacute;cart</ind></trd></trdgrp> 
</drv>
<drv mrk="for.0vagi">
  <kap><tld/>vagi</kap>
  <snc>
    <dif>
      Malproksimen vagi:
      <ekz>
        <tld/>vagas flanken &ccirc;iam liaj pensoj al la
        gepatra domo<fnt>Z</fnt>.
      </ekz> 
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>divaguer</trd>,
    <trd>vagabonder</trd></trdgrp> 
</drv>
<drv mrk="for.0veturi">
  <kap><tld/>veturi<fnt>Z</fnt></kap>
  <snc>
    <dif>
      <tld/>iri veturante.  
    </dif>
  </snc>
  <trd lng="de">wegfahren</trd>
  <trd lng="fr">partir</trd>
</drv>
</subart>
</art>
