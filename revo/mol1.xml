  <trd lng="cs">mol</trd>
  <trd lng="da">mol</trd>
  <trd lng="de">Mol</trd>
  <trd lng="en">mole</trd>
  <trd lng="es">mol</trd>
  <trd lng="fr">mole</trd>
  <trd lng="hu">m&oacute;l</trd>
  <trd lng="it">mole</trd>
  <trd lng="nl">mol</trd>
  <trd lng="pl">mol</trd>
  <trd lng="ru">&c_m;&c_o;&c_l;&c_mol;</trd>
  <trd lng="tr">mol</trd>
