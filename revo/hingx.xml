<vortaro>
<art mrk="$Id: hingx.xml,v 1.11 2021/06/08 06:51:14 revo Exp $">
<kap>
  <rad>hin&gcirc;</rad>/o
</kap>
<drv mrk="hingx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <uzo tip="stl">RAR</uzo>
  <fnt><bib>PV</bib></fnt>
  <snc>
    <dif>
      <ref tip="dif" cel="hok.pord0o">Pordhoko</ref>:
      <ekz>
        la fenestrokovriloj pendis &ccirc;e <tld/>oj el ligno
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=489247"
              >Trunkodomo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <nom>Arne</nom>, sa&gcirc;a kaj klarvida knabo estas la &ccirc;efheroo de la rakonto, iuspeca &leftquot;<tld/>o&rightquot;
        &ccirc;irka&ubreve; kiu turni&gcirc;as la rakonto
        <fnt>
          <vrk><url ref="http://www.esperanto.net/literaturo/roman/libr/arnecefidreczm.html"
              >Arne, la &ccirc;efido - esperanto.net</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Angel <klr>(T&uuml;r)</klr></trd>,
    <trd>T&uuml;rangel</trd>,
    <trd>T&uuml;rhaken</trd>
  </trdgrp>
  <trd lng="fr">gond</trd>
  <trd lng="hu">sarokvas<klr tip="ind"> (ajt&oacute;n)</klr></trd>
  <trd lng="nl">scharnier</trd>
  <trd lng="pl">zawiasa</trd>
  <trdgrp lng="pt">
   <trd>gonzo</trd>, 
   <trd>dobradi&ccedil;a</trd> 
  </trdgrp>
  <trd lng="ru">&c_d;&c_v;&c_je;&c_r;&c_n;&c_o;&c_j; &c_k;&c_r;&c_ju;&c_ch;&c_o;&c_k;</trd>
</drv>
