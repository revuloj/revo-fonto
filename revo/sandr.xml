<art mrk="$Id: sandr.xml,v 1.26 2023/09/03 12:09:44 revo Exp $">
<kap>
  <rad>sandr</rad>/o
</kap>

<drv mrk="sandr.0o">
  <kap><tld/>o</kap>
    <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="fisx.0o" lst="voko:fi&scirc;oj">Fi&scirc;o</ref>
      vivanta en nesala akvo, &gcirc;is unu metro longa, kun du apartaj
      dorsna&gcirc;iloj, kelkaj specioj estas tre bongustaj;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio <ref tip="malprt" cel="perked.0oj">perkedoj</ref>
      (<trd lng="la">Sander</trd>):
      <ekz>
        la&ubreve;longe de la rivero oni elprenis plurmil tunojn da mortintaj fi&scirc;oj (plejparte
        &ccirc;inaj karpoj, bramoj, siluroj, <tld/>oj, karpoj)
        <fnt><aut>Katalin Smid&eacute;liusz</aut>: 
          <vrk><url ref="&Monato;/2000/006912.php">Cinike cianide -- &ccirc;u respondecas
          Okcidento?</url></vrk>,
          <bib>Monato</bib>
          <lok>2000/04, p. 15</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/e/e8/Sander_lucioperca_Hungary.jpg/640px-Sander_lucioperca_Hungary.jpg" prm="CC BY 3.0"><tld/>o (Sander lucioperca), Hortobagy, Hungarujo
        <fnt>
          <aut>Harka, Akos</aut>
          <lok><url ref="&WCW;/File:Sander_lucioperca_Hungary.jpg">&WCW;/File:Sander_lucioperca_Hungary.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_d;&c_a;&c_k;</trd>
  <trd lng="bg">&c_b;&c_ja;&c_l;&c_a; &c_r;&c_i;&c_b;&c_a;</trd>
  <trd lng="cs">cand&aacute;t obecn&yacute;</trd>
  <trd lng="de">Zander</trd>
  <trd lng="fr">sandre</trd>
  <trd lng="hu">fogass&uuml;ll&odblac;</trd>
  <trdgrp lng="it">
    <trd>lucioperca</trd>, 
    <trd>luccioperca</trd>, 
    <trd>sandra</trd>
  </trdgrp>
  <trd lng="ja">&#x30DB;&#x30BD;&#x30B9;&#x30BA;&#x30AD;</trd>
  <trdgrp lng="nl">
    <trd>snoekbaars</trd>,
    <trd>zander</trd>
  </trdgrp>
  <trd lng="pl">sandacz</trd>
  <trd lng="pt">sandra</trd>
  <trd lng="ru">&c_s;&c_u;&c_d;&c_a;&c_k;</trd>
  <trd lng="sk">zub&aacute;&ccaron;</trd>
  <trd lng="uk">&c_s;&c_u;&c_d;&c_a;&#x43A;</trd>
</drv>
</art>
