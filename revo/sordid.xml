<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sordid.xml,v 1.7 2019/10/19 16:10:19 revo Exp $">
<kap>
  <rad>sordid</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sordid.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="sordid.0a.KOMUNE">
    <dif>
      <ref tip="dif" cel="pur.mal0a.KOMUNE">Malpurega</ref>:
      <ekz>
        magistrato malkonstruis la <tld/>ajn apartamentojn 
        kaj konstruigis pli bonajn domarojn
        <fnt>
          <aut>G. Macaoidh</aut>,
          <vrk><url ref="https://www.monato.be/2006/008699.php">
          Kara antikva malpura Dublino</url></vrk>,
          <bib>Monato</bib>,
          <lok>2006:07, p. 13a</lok>
        </fnt>;
      </ekz>
      <ekz>
        spioni &ccirc;e sakstrato reziduojn de <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007198.html">Poezio sed ne poemoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>verdreckt</trd>,
    <trd>verwahrlost</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>filthy</trd>,
    <trd>sordid</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>neomalen&yacute;</trd>,
    <trd>podl&yacute;</trd>,
    <trd>sprost&yacute;</trd>,
    <trd>&scaron;pinav&yacute;</trd>
  </trdgrp>
  <trd lng="fr">sordide</trd>
  <trdgrp lng="pl">
    <trd>brudny</trd>,
    <trd>niechlujny</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>podl&yacute;</trd>,
    <trd>uf&uacute;&lcaron;an&yacute;</trd>,
    <trd>&scaron;pinav&yacute;</trd>
  </trdgrp>
</drv>
</art>

