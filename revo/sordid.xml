<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sordid.xml,v 1.13 2025/04/05 19:27:39 revo Exp $">
<kap>
  <rad>sordid</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sordid.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="sordid.0a.KOMUNE">
    <dif>
      <ref tip="dif" cel="pur.mal0a.KOMUNE">Malpurega</ref>:
      <ekz>
        magistrato malkonstruis la <tld/>ajn apartamentojn
        kaj konstruigis pli bonajn domarojn
        <fnt>
          <aut>Garbhan MacAoidh</aut>,
          <vrk><url ref="&Monato;/2006/008699.php">
          Kara antikva malpura Dublino</url></vrk>,
          <bib>Monato</bib>,
          <lok>2006/07, p. 13</lok>
        </fnt>;
      </ekz>
      <ekz>
        spioni &ccirc;e sakstrato reziduojn de <tld/>o
        <fnt>
          <aut>Grigori Arosev</aut>
            <vrk><url ref="&Monato;/2000/007198.php">Poezio sed ne poemoj</url></vrk>
            <bib>Monato</bib>
            <lok>2000/12, p. 28</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>malpura</ke>, <ke>a&#265;a</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_b;&c_r;&c_u;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_a;&c_h;&c_a;&c_j;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verdreckt</trd>,
    <trd>verwahrlost</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>filthy</trd>,
    <trd>sordid</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>neomalen&yacute;</trd>,
    <trd>podl&yacute;</trd>,
    <trd>sprost&yacute;</trd>,
    <trd>&scaron;pinav&yacute;</trd>
  </trdgrp>
  <trd lng="fr">sordide</trd>
  <trdgrp lng="pl">
    <trd>brudny</trd>,
    <trd>niechlujny</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>podl&yacute;</trd>,
    <trd>uf&uacute;&lcaron;an&yacute;</trd>,
    <trd>&scaron;pinav&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_r;&c_u;&c_d;&#1085;&c_i;&c_j;</trd>,
    <trd>&#1086;&c_g;&c_i;&c_d;&#1085;&c_i;&c_j;</trd>,
    <trd>&#1084;&c_je;&c_r;&c_z;&c_je;&#1085;&#1085;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#27745;&#31229; <pr>w&#363;hu&#236;</pr></trd>,
    <trd>&#27745;&#31330; <pr>w&#363;hu&#236;</pr></trd>,
    <trd>&#27745;&#31967; <pr>w&#363;z&#257;o</pr></trd>,
    <trd>&#40823;&#28609; <pr>w&#242;zhu&#243;</pr></trd>,
    <trd>&#40844;&#27978; <pr>w&#242;zhu&#243;</pr></trd>
  </trdgrp>
</drv>
</art>

