<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: juli.xml,v 1.44 2023/12/03 07:09:19 revo Exp $">
<kap>
  <ofc>*</ofc> <rad>Juli</rad>/o,
  <var><kap><rad var="j">juli</rad>/o</kap></var>
</kap>
<drv mrk="juli.0o">
  <kap><ofc>*</ofc><tld/>o,
  <var><kap><tld var="j"/>o</kap></var></kap>
  <snc mrk="juli.0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Sepa <ref tip="lst" cel="monat.0o.KAL" lst="voko:monatoj" val="7">monato</ref> de la <ref tip="malprt" cel="jar.0o.KAL">jaro</ref> la&ubreve; la Gregoria kalendaro, inter <ref cel="juni.0o">Junio</ref>
      kaj <ref cel="auxgus.0o">A&ubreve;gusto</ref>:
      <ekz>
        la dek du monatoj de l' kristana jaro estas: Januaro,
        <klr>[&#x2026;]</klr> Majo, Junio, <tld/>o, A&ubreve;gusto,
        <klr>[&#x2026;]</klr>
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        Varsovio, <tld/>o 1905
        <fnt>
          <bib>F</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la aldono 1908 aperos en <tld/>o
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        la 4-an de <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        en Kolonjo la milito lin haltigis la 30-an de <tld lit="j"/>o
        <fnt>
          <bib>VivZam</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="januar.0o.RIM">Januaro, Rim.</ref>
  <ref tip="hom" cel="juli1.0o">Julio (nomo)</ref>
  <trd lng="af">Julie</trd>
  <trd lng="be">&c_l;&c_ib;&c_p;&c_je;&c_n;&c_mol;</trd>
  <trd lng="bg">&c_ju;&c_l;&c_i;</trd>
  <trd lng="bo">&#xF5F;&#xFB3;&#xF0B;&#xF56;&#xF0B;&#xF56;&#xF51;&#xF74;&#xF53;&#xF0B;&#xF54;&#xF0B;</trd>
  <trd lng="br">Gouere</trd>
  <trd lng="cs">&ccaron;ervenec</trd>
  <trd lng="de">Juli</trd>
  <trd lng="el">&Jota;&omikron;&ypsilon_ton;&lambda;&jota;&omikron;&sigma_fina;</trd>
  <trd lng="en">July</trd>
  <trd lng="es">julio</trd>
  <trd lng="fa">&f_zh;&a_w;&a_y_hamza;&f_y;&a_h;</trd>
  <trd lng="fr">juillet</trd>
  <trd lng="fy">july</trd>
  <trd lng="he">&yod;&vav;&lamed;&yod;</trd>
  <trd lng="hu">j&uacute;lius</trd>
  <trd lng="id">Juli</trd>
  <trd lng="io">julio</trd>
  <trd lng="it">luglio</trd>
  <trd lng="ja">&#x4E03;&#x6708; <pr>&#x3057;&#x3061;&#x304C;&#x3064;</pr></trd>
  <trdgrp lng="jbo">
    <trd>zelma'i</trd>,
    <trd>mlajukma'i</trd>
  </trdgrp>
  <trd lng="nl">juli</trd>
  <trd lng="pl">lipiec</trd>
  <trd lng="pt">julho</trd>
  <trd lng="ro">iulie</trd>
  <trd lng="ru">&c_i;&c_ju;&c_l;&c_mol;</trd>
  <trd lng="sk">j&uacute;l</trd>
  <trd lng="sv">juli</trd>
  <trd lng="tok">tenpo mun nanpa luka tu</trd>
  <trd lng="tr">temmuz</trd>
  <trd lng="uk">&#x42E;&#x43B;&c_ib;&c_j;</trd>
  <trd lng="vo">yulul</trd>
  <trdgrp lng="zh">
    <trd>&#x4E03;&#x6708; <pr>Q&#x12B;yu&#xE8;</pr></trd>,
    <trd>&#x4E03;&#x6708;&#x4EFD; <pr>q&#x12B;yu&#xE8;f&#xE8;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="juli.0a">
  <kap><tld lit="j"/>a</kap>
  <snc>
    <dif>
      Okazanta en <tld/>o, rilata al <tld/>o:
      <ekz>
        en iu varma <tld lit="j"/>a posttagmezo
        <fnt>
          <aut>Julian Modest</aut>
            <vrk><url ref="&Monato;/2001/007140.html">Vetiha</url></vrk>
            <bib>Monato</bib>
            <lok>2001/02, p. 26</lok>
          </fnt>;
      </ekz>
      <ekz>
        reage al la enkonduko <tld lit="j"/>a
        <klr>(de la <tld/>a numero)</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Rita Galmache</aut>
          <vrk><url ref="&Monato;/2008/009556.php">Angla
          Germanio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="juli1.0a">Julia</ref>
  <trd lng="be">&c_l;&c_ib;&c_p;&c_je;&c_n;&c_mol;&c_s;&c_k;&c_ib;</trd>
  <trd lng="io">juliala</trd>
  <trd lng="ja">&#x4E03;&#x6708;&#x306E; <pr>&#x3057;&#x3061;&#x304C;&#x3064;&#x306E;</pr></trd>
  <trd lng="uk">&#x44E;&#x43B;&c_ib;&c_a;&#x43D;&c_s;&#x44C;&#x43A;&c_i;&c_j; (&#x43F;&c_r;&#x43E;
  &#x43A;&c_a;&#x43B;&c_je;&#x43D;&c_d;&c_a;&c_r;)</trd>
  <trd lng="vo">yululik</trd>
</drv>


</art>
