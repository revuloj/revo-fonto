<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>juli</rad>/o
</kap>
<drv mrk="juli.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Sepa monato de la jaro.
    </dif>
  </snc>
  <ref tip="vid" cel="januar.0o">januaro, Rim.</ref>
  <trd lng="be">&c_l;&c_ib;&c_p;&c_je;&c_n;&c_mol;</trd>
  <trd lng="cs">&ccaron;ervenec</trd>
  <trd lng="de">Juli</trd>
  <trd lng="el">&Jota;&omikron;&ypsilon_ton;&lambda;&jota;&omikron;&sigma_fina;</trd>
  <trd lng="en">July</trd>
  <trd lng="es">julio</trd>
  <trd lng="fr">juillet</trd>
  <trd lng="he">&yod;&vav;&lamed;&yod;</trd>
  <trd lng="hu">j&uacute;lius</trd>
  <trd lng="it">luglio</trd>
  <trd lng="nl">juli</trd>
  <trd lng="pl">lipiec</trd>
  <trd lng="pt">julho</trd>
  <trd lng="ru">&c_i;&c_ju;&c_l;&c_mol;</trd>
  <trd lng="sv">juli</trd>
  <trd lng="tr">temmuz</trd>
  <trd lng="vo">yulul</trd>
</drv>
</art>
