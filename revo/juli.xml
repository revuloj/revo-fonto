<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: juli.xml,v 1.35 2019/06/30 18:21:02 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>juli</rad>/o
</kap>
<drv mrk="juli.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="juli.0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Sepa <ref tip="lst" cel="monat.0o.KAL" lst="voko:monatoj"
      val="7">monato</ref> de la <ref tip="malprt"
      cel="jar.0o.KAL">jaro</ref> la&ubreve; la Gregoria kalendaro, inter junio
      kaj a&ubreve;gusto:
      <ekz>
        la dek du monatoj de l' kristana jaro estas: Januaro,
        <klr>[&#8230;]</klr> Majo, Junio, <tld lit="J"/>o, A&ubreve;gusto,
        <klr>[&#8230;]</klr>
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        Varsovio, <tld lit="J"/>o 1905
        <fnt>
          <bib>F</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la aldono 1908 aperos en <tld lit="J"/>o
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        la 4-an de <tld lit="J"/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        en Kolonjo la milito lin haltigis la 30-an de <tld/>o
        <fnt>
          <bib>VivZam</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="januar.0o.RIM">januaro, Rim.</ref>
  <trd lng="af">Julie</trd>
  <trd lng="be">&c_l;&c_ib;&c_p;&c_je;&c_n;&c_mol;</trd>
  <trd lng="bg">&c_ju;&c_l;&c_i;</trd>
  <trd lng="br">Gouere</trd>
  <trd lng="cs">&ccaron;ervenec</trd>
  <trd lng="de">Juli</trd>
  <trd lng="el">&Jota;&omikron;&ypsilon_ton;&lambda;&jota;&omikron;&sigma_fina;</trd>
  <trd lng="en">July</trd>
  <trd lng="es">julio</trd>
  <trd lng="fa">&f_zh;&a_w;&a_y_hamza;&f_y;&a_h;</trd>
  <trd lng="fr">juillet</trd>
  <trd lng="fy">july</trd>
  <trd lng="he">&yod;&vav;&lamed;&yod;</trd>
  <trd lng="hu">j&uacute;lius</trd>
  <trd lng="id">Juli</trd>
  <trd lng="io">julio</trd>
  <trd lng="it">luglio</trd>
  <trd lng="ja">&#19971;&#26376; [<ind>&#12375;&#12385;&#12364;&#12388;</ind>]</trd>
  <trdgrp lng="jbo">
    <trd>zelma'i</trd>,
    <trd>mlajukma'i</trd>
  </trdgrp>
  <trd lng="nl">juli</trd>
  <trd lng="pl">lipiec</trd>
  <trd lng="pt">julho</trd>
  <trd lng="ro">iulie</trd>
  <trd lng="ru">&c_i;&c_ju;&c_l;&c_mol;</trd>
  <trd lng="sk">j&uacute;l</trd>
  <trd lng="sv">juli</trd>
  <trd lng="tp">tenpo mun nanpa luka tu</trd>
  <trd lng="tr">temmuz</trd>
  <trd lng="vo">yulul</trd>
</drv>

<drv mrk="juli.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Okazanta en <tld/>o, rilata al <tld/>o:
      <ekz>
        en iu varma <tld/>a posttagmezo
        <fnt>
          <bib>Monato</bib>
          <vrk><url
          ref="http://steloj.de/esperanto/monato2/007140.html">de Julian
          Modest</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        reage al la enkonduko <tld/>a
        <klr>(de la <tld/>a numero)</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Rita Galmache</aut>
          <vrk><url ref="&Monato;/2008/009556.php">Angla
          Germanio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="io">juliala</trd>
  <trd lng="ja">&#19971;&#26376;&#12398; [<ind>&#12375;&#12385;&#12364;&#12388;&#12398;</ind>]</trd>
  <trd lng="vo">yululik</trd>
</drv>


</art>
