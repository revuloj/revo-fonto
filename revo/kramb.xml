<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kramb.xml,v 1.20 2023/09/01 15:46:44 revo Exp $">
<kap>
  <rad>kramb</rad>/o
</kap>
<drv mrk="kramb.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      Plurjara <ref tip="lst" cel="herb.0o.BOT" lst="voko:brasikoparencoj">herbo</ref>
      kun longa <ref tip="prt" cel="bet.0o.radiko">beto</ref>,
      parenca al brasiko, sed kun blankaj floretoj kaj
      dupartaj silikvoj kun po unu globforma semgrajno;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref> el familio <ref tip="malprt" cel="brasik1.0oj">brasikacoj</ref>
      (<trd lng="la">Crambe</trd>):
      <ekz>
         la mara <tld/>o estas unu el la simbolaj specioj, &gcirc;i estas
         praplanto de &ccirc;iuj kultivataj brasikoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=465184">Kabo
          Blanc-Nez</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la Tatara <tld/>o estas sova&gcirc;a planto de la stepoj de orienta
        kaj suda E&ubreve;ropo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=565035">Krambo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_r;&c_a;&c_n; <klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trd lng="de">Meerkohl</trd>
  <trd lng="en">sea kale</trd>
  <trd lng="fa">&a_s;&f_p;&f_y;&a_d;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>cramb&eacute;<klr> (maritime)</klr></trd>,
    <trd>chourbe</trd>,
    <trd>chou marin</trd>
  </trdgrp>
  <trd lng="hu">t&aacute;torj&aacute;n</trd>
  <trd lng="ja">&#x30CF;&#x30DE;&#x30CA;</trd>
  <trd lng="pl">modrak <klr>(kapusta morska)</klr></trd>
  <trdgrp lng="pt">
    <trd>crambe</trd>,
    <trd>couve-marinha</trd>
  </trdgrp>
  <trd lng="ro">varz&abreve; de mare</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_t;&c_r;&c_a;&c_n;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_s;&c_k;&c_a;&c_ja; &c_k;&c_a;&c_p;&c_u;&c_s;&c_t;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_a;&c_t;&c_r;&c_a;&#x43D;</trd>,
    <trd>&#x43C;&#x43E;&c_r;&c_s;&#x44C;&#x43A;&c_a; &#x43A;&c_a;&#x43F;&c_u;&c_s;&c_t;&c_a;</trd>
  </trdgrp>
</drv>
</art>
