<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: sxtern.xml,v 1.6 2023/12/15 01:36:56 afido Exp $">
    <kap>
      <rad>&scirc;tern</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="sxtern.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxtern.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Bone fluganta mar<ref tip="lst" lst="voko:birdoj" cel="bird.0o.ZOO">birdo</ref>
      kun longaj flugiloj, akra beko, ordinare dupinta vosto;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="lared.0oj">laredoj</ref>
      (<trd lng="la">Sterna</trd>):
      <ekz>
        kelkaj <tld/>oj metis siajn ovojn apud la lago
        <fnt>
          <aut>H. Laxness, trad. B. Ragnarsson</aut>
          <vrk>Sendependaj homoj</vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        Arkta <tld/>o <klr>[vivas]</klr> kolonie en arktaj kaj subarktaj regionoj de
        E&ubreve;ropo, Azio, kaj Norda Ameriko
        <fnt>
          <vrk><url ref="&Viki;/Arkta_%C5%9Dterno">Arkta &scirc;terno</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/0/0f/Common_Tern_5.jpg/409px-Common_Tern_5.jpg" prm="CC BY 2.0">Hirunda
    <tld/>o (Sterna hirundo)
        <fnt>
          <aut>Tony Hisgett, Flickr</aut>
          <lok><url ref="&WCW;/File:Common_Tern_5.jpg">&WCW;/File:Common_Tern_5.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
    <trd lng="be">&c_r;&c_y;&c_b;&c_a;&c_ch;&c_k;&c_a; <klr tip="amb">(&c_p;&c_t;&c_u;&c_sh;&c_k;&c_a;)</klr></trd>
    <trd lng="de">Seeschwalbe</trd>
    <trd lng="en">Sterna</trd>
    <trd lng="fr">sterne</trd>
    <trd lng="pl">rybitwa</trd>
    <trd lng="uk">&#x43A;&c_r;&#x44F;&c_ch;&#x43E;&#x43A;</trd>
</drv>
</art>

