<drv mrk="mark.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="mark.0i.iu">
    <dif>
      <klr>(iu)</klr> Fari, meti <tld/>on sur io:
      <ekz>
        marku per litero <nom>Tav</nom> la fruntojn de tiuj homoj
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 9:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>is per peco da kreto sur la pordo grandan krucon
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        reto de <tld/>itaj <klr>[&#x2026;]</klr> montbiciklaj vojoj
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Steloj;monato2/007151.html">900 km tra montbicikla
          paradizo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        por sekvi ilian migradon oni <tld/>as la linkojn <klr>[&#x2026;]</klr> per specialaj
        dissendiloj
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2012/010875.php">Pereon de specioj malhelpas
          homoj</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="mark.0i.io">
    <dif>
      <klr>(io)</klr> Servi kiel <ref cel="mark.0o.signo"><tld/>o<sncref/></ref>:
      <ekz>
        la 6a de januaro <tld/>as <klr>[&#x2026;]</klr> la finon de la festoperiodo
        <fnt>
          <bib>Monato</bib>
          <aut>Sylvain Lelarge</aut>
          <vrk><url ref="&Monato;/2004/008496.html">La argilaj najbaretoj de Jesuo
          provenca tradicio</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la &ccirc;ina kulturo la Printempa Festo estas <tld/>ata per kuna vesperman&gcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="&Monato;/2010/010021.php">Migrado multmiliona</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        barilo <klr>[&#x2026;]</klr> <tld/>as la eniron al unu el la funkciulaj kvartaloj
        <fnt>
          <bib>Monato</bib>
          <aut>Ulrich Lins</aut>
          <vrk><url ref="&Monato;/2005/008536.php">Nokta aventuro en
          Pjongjango</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        li konigas al ni iun sperton, kiu <tld/>as permanente en lia memoro la etoson de
        tiu lando
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2007/009082.php">Majstra verko de a&ubreve;toro preska&ubreve;
          nekonata</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_r;&c_k;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_z;&c_n;&c_a;&c_ch;&c_a;&c_c;&c_mol;</trd>,
