<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mark</rad>/o
</kap>
<drv mrk="mark.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Speciala signo metita sur io kaj servanta por rekonigi kaj
      distingigi &gcirc;in:
      <ekz>
        <tld/>o sur fota&jcirc;o, en libro;
      </ekz>
      <ekz>
        komerca <tld/>o, fabrik<tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Marke</trd>,
      <trd>Markierung</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>jegy</trd>,
      <trd>m&aacute;rka</trd>
    </trdgrp>
    <trd lng="nl">merkteken</trd>
    <trd lng="pt">marca</trd>
  </snc>
  <snc>
    <dif>
      Objekto havanta specialan signon:
      <ekz>
        <ind>po&scirc;t<tld/>o</ind>,
        <trd lng="hu">postab&eacute;lyeg</trd>
        <trd lng="nl">postzegel</trd>
      </ekz>
      <ekz>
        lud<tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Marke</trd>,
      <trd>Markierung</trd>
    </trdgrp>
    <trd lng="hu">b&eacute;lyeg</trd>
  </snc>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="unu.mon0o">Monunuo</ref> de Estonio. 
      Iama monunuo de Germanujo kaj Finnlando.
    </dif>
    <trd lng="de">Mark</trd>
    <trd lng="hu">m&aacute;rka</trd>
    <trd lng="pt">marco</trd>
  </snc>
  <trd lng="cs">zn&aacute;mka</trd>
  <trd lng="en">mark</trd>
  <trd lng="nl">mark</trd>
  <trd lng="ru">&c_m;&c_a;&c_r;&c_k;&c_a;</trd>
</drv>

<drv mrk="mark.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari, meti <tld/>on sur io:
      <ekz>
        <tld/>i per peco da kreto krucon sur la pordo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i tola&jcirc;on, pa&gcirc;on, dehakotan arbon;
      </ekz>
      <ekz>
        <tld/>i krajone la komposterarojn.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>megjel&ouml;l</trd>,
    <trd>kijel&ouml;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>aanduiden</trd>,
    <trd>aanstrepen</trd>
  </trdgrp>
  <trd lng="pt">marcar</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_m;&c_je;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_je;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_je;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_k;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="mark.brul0i">
  <kap>brul<tld/>i</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
    Fari <tld/>on per brulanta fera&jcirc;o sur la ha&ubreve;to de bruto, 
    por indiki ties posedatecon.
    </dif>
  </snc>
  <trd lng="en">brand</trd>
  <trd lng="hu">megb&eacute;lyegez<klr tip="ind"> (billoggal)</klr></trd>
  <trd lng="pt">marcar &agrave; fogo</trd>
</drv>

<drv mrk="mark.brul0o">
  <kap>brul<tld/>o</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
    Signalo brul<tld/>ita sur la ha&ubreve;to de brutoj.
    </dif>
  </snc>
  <trd lng="en">brand</trd>
  <trd lng="hu">billog</trd>
  <trd lng="pt">marca de fogo</trd>
</drv>

<drv mrk="mark.brul0ilo">
  <kap>brul<tld/>ilo</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
    Ilo uzata por brul<tld/>i. 
    </dif>
  </snc>
  <trd lng="en">branding iron</trd>
  <trd lng="hu">b&eacute;lyegz&odblac;vas</trd>
  <trd lng="pt">marcador</trd>
</drv>

</art>
