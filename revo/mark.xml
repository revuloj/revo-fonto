<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mark</rad>/o
</kap>

<drv mrk="mark.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="mark.0o.sign">
    <dif>
      Speciala signo metita sur io kaj servanta por rekonigi kaj
      distingigi &gcirc;in:
      <ekz>
        <tld/>o sur fotografa&jcirc;o, en libro;
      </ekz>
      <ekz>
        komerca <tld/>o, fabrik<tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_m;&c_a;&c_r;&c_k;&c_a;</trd>,
      <trd>&c_a;&c_d;&c_z;&c_n;&c_a;&c_k;&c_a;</trd>,
      <trd>&c_p;&c_a;&c_z;&c_n;&c_a;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="br">merk</trd>
    <trdgrp lng="de">
      <trd>Marke</trd>,
      <trd>Markierung</trd>
    </trdgrp>
    <trd lng="es">marca</trd>
    <trdgrp lng="hu">
      <trd>jegy</trd>,
      <trd>m&aacute;rka</trd>
    </trdgrp>
    <trd lng="nl">merkteken</trd>
    <trd lng="pt">marca</trd>
    <trdgrp lng="fr">
      <trd>marque</trd><trd>rep&egrave;re</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_m;&c_a;&c_r;&c_k;&c_a;</trd>,
      <trd>&c_m;&c_je;&c_t;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="mark.0o.objekt">
    <dif>
      Objekto havanta specialan signon:
      <ekz>
        <ind>po&scirc;t<tld/>o</ind>,
        <trd lng="be">&c_p;&c_a;&c_sh;&c_t;&c_o;&c_v;&c_a;&c_ja;
        <ind>&c_m;&c_a;&c_r;&c_k;&c_a;</ind></trd>
        <trd lng="bg">&c_p;&c_o;&c_shch;&c_je;&c_n;&c_s;&c_k;&c_a;
        &c_m;&c_a;&c_r;&c_k;&c_a;</trd>
        <trd lng="br">timbr</trd>
        <trd lng="de">Briefmarke</trd>
        <trdgrp lng="es">
          <trd>timbre</trd>,
          <trd>sello</trd>
        </trdgrp>
        <trd lng="fr">timbre(-poste)</trd>
        <trd lng="hu">postab&eacute;lyeg</trd>
        <trd lng="nl">postzegel</trd>
        <trdgrp lng="pt">
          <trd>marca</trd>,
          <trd>selo</trd>
        </trdgrp>
        <trd lng="ru">&c_p;&c_o;&c_ch;&c_t;&c_o;&c_v;&c_a;&c_ja;
        <ind>&c_m;&c_a;&c_r;&c_k;&c_a;</ind></trd>
      </ekz>
      <ekz>
        lud<tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_m;&c_a;&c_r;&c_k;&c_a;</trd>,
      <trd>&c_m;&c_a;&c_r;&c_k;&c_ib;&c_r;&c_o;&c_w;&c_k;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Marke</trd>,
      <trd>Markierung</trd>
    </trdgrp>
    <trd lng="hu">b&eacute;lyeg</trd>
    <trd lng="nl">zegel</trd>
  </snc>
  <rim mrk="mark.0o.RIM_HOM">
    <ref tip="hom" cel="mark1.0o">Marko</ref> estas anka&ubreve;
    voknomo.
    Kelkaj monunuoj estas nomataj <ref tip="hom" 
    cel="mark2.0o">marko</ref>.
  </rim>
  <trdgrp lng="bg">
    <trd>&c_m;&c_a;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_n;&c_a;&c_k;</trd>,
    <trd>&c_b;&c_je;&c_l;&c_ja;&c_z;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">zn&aacute;mka</trd>
  <trd lng="en">mark</trd>
  <trd lng="ru">&c_m;&c_a;&c_r;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>n&aacute;ves&tcaron;</trd>,
    <trd>ozna&ccaron;enie</trd>,
    <trd>&scaron;t&iacute;tok</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>n&eacute;vus</trd>,
    <trd>otisk</trd>,
    <trd>pohmo&zcaron;d&#65533;&#8250;nina</trd>,
    <trd>skvrna</trd>,
    <trd>znam&eacute;nko</trd>
  </trdgrp>
</drv>

<drv mrk="mark.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari, meti <tld/>on sur io:
      <ekz>
        <tld/>i per peco da kreto krucon sur la pordo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i tola&jcirc;on, pa&gcirc;on, dehakotan arbon;
      </ekz>
      <ekz>
        <tld/>i krajone la komposterarojn.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_r;&c_k;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_z;&c_n;&c_a;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_z;&c_n;&c_a;&c_ch;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_o;&c_t;&c_b;&c_je;&c_l;&c_ja;&c_z;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_o;&c_z;&c_n;&c_a;&c_ch;&c_a;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_k;&c_i;&c_r;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="br">merka&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>ozna&ccaron;it</trd>,
    <trd>poznamenat</trd>,
    <trd>zatrhnout</trd>,
    <trd>za&scaron;krtnout</trd>,
    <trd>zna&ccaron;it</trd>,
    <trd>zna&ccaron;kovat</trd>
  </trdgrp>
  <trd lng="de">markieren</trd>
  <trd lng="es">marcar</trd>
  <trd lng="fr">marquer</trd>
  <trdgrp lng="hu">
    <trd>megjel&ouml;l</trd>,
    <trd>kijel&ouml;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>aanduiden</trd>,
    <trd>aanstrepen</trd>
  </trdgrp>
  <trd lng="pt">marcar</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_m;&c_je;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_je;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_je;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_k;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>zna&ccaron;i&tcaron;</trd>,
    <trd>zna&ccaron;kova&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="mark.brul0i">
  <kap>brul<tld/>i</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Fari <tld/>on per brulanta fera&jcirc;o sur la ha&ubreve;to de
      bruto,
      por indiki ties posedatecon.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_l;&c_je;&c_j;&c_m;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_l;&c_ja;&c_j;&c_m;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_t;&c_a;&c_w;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_zh;&c_i;&c_g;&c_o;&c_s;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_k;&c_l;&c_je;&c_j;&c_m;&c_o;&c_s;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="cs">vyp&aacute;lit zna&ccaron;ku</trd>
  <trd lng="de">brandmarken</trd>
  <trd lng="en">brand</trd>
  <trd lng="es">marcar a fuego</trd>
  <trd lng="fr">marquer au fer</trd>
  <trd lng="hu">megb&eacute;lyegez<klr tip="ind">
  (billoggal)</klr></trd>
  <trd lng="nl">brandmerken</trd>
  <trd lng="pt">marcar a fogo</trd>
  <trd lng="ru">&c_k;&c_l;&c_je;&c_j;&c_m;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">vyp&aacute;li&tcaron; zna&ccaron;ku</trd>
</drv>

<drv mrk="mark.brul0o">
  <kap>brul<tld/>o</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Signalo brul<tld/>ita sur la ha&ubreve;to de brutoj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_l;&c_ja;&c_j;&c_m;&c_o;</trd>,
    <trd>&c_t;&c_a;&c_w;&c_r;&c_o;</trd>
  </trdgrp>
  <trd lng="bg">&c_k;&c_l;&c_je;&c_j;&c_m;&c_o;</trd>
  <trd lng="cs">vyp&aacute;len&aacute; zna&ccaron;ka</trd>
  <trd lng="de">Brandzeichen</trd>
  <trd lng="en">brand</trd>
  <trd lng="es">marca de fuego</trd>
  <trd lng="hu">billog</trd>
  <trd lng="nl">brandmerk</trd>
  <trd lng="pt">marca de fogo</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_l;&c_je;&c_j;&c_m;&c_o;</trd>,
    <trd>&c_t;&c_a;&c_v;&c_r;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">vyp&aacute;len&aacute; zna&ccaron;ka</trd>
</drv>

<drv mrk="mark.brul0ilo">
  <kap>brul<tld/>ilo</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Ilo uzata por brul<tld/>i.
    </dif>
  </snc>
  <trd lng="de">Brandeisen</trd>
  <trd lng="en">branding iron</trd>
  <trd lng="es">hierro de marcar a fuego<klr> (el ganado)</klr></trd>
  <trd lng="hu">b&eacute;lyegz&odblac;vas</trd>
  <trd lng="nl">schroei-ijzer</trd>
  <trd lng="pt">marcador</trd>
  <trd lng="ru">&c_k;&c_l;&c_je;&c_j;&c_m;&c_o;</trd>
</drv>

<drv mrk="mark.posxt0o">
  <kap>po&scirc;t<tld/>o,
  <var><kap><ofc>*</ofc>signo de po&scirc;to <fnt><bib>F</bib>,
  <lok>Universala Vortaro, <url
  ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5168">sign'o
  de po&scirc;t'o </url></lok></fnt></kap></var>
  </kap>
  <snc mrk="mark.posxt0o.LETERO">
    <dif>
      Po&scirc;t<tld/>o estas papera marko kun difinita valoro. 
      &Gcirc;i estas eldonita de po&scirc;toj por faciligi la pagadon de
      afranko.
    </dif>
    <trd lng="bg">&c_p;&c_o;&c_shch;&c_je;&c_n;&c_s;&c_k;&c_a;
    &c_m;&c_a;&c_r;&c_k;&c_a;</trd>
    <trd lng="de">Briefmarke</trd>
    <trd lng="en">postage stamp</trd>
  </snc>
</drv>
</art>
