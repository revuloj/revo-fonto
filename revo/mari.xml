<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">

<kap>
  <rad>Mari</rad>/o
  <fnt>
    <bib>PIV1</bib>
  </fnt>
</kap>

<drv mrk="mari.0o">
  <kap><tld/>o</kap>
  <snc mrk="mari.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        <tld/>o havas sian sekreton pri kiu scias neniu
        <fnt>
          <aut>&Scirc;. Urban</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr46/papilioj.html">
          Mario kaj la papilioj</url></vrk>,
          <lok>Nica Literatura Revuo, 1959-97 &gcirc;is 08 (4/6)</lok>
        </fnt>.
      </ekz>
    </dif>
  <trdgrp lng="ca">
    <trd>Mari</trd>,
    <trd>M&agrave;rius</trd>
  </trdgrp>
  <trd lng="en">Mario</trd>
  <trdgrp lng="fr">
    <trd>Mario</trd>,
    <trd>Marius</trd>
  </trdgrp>
  <trd lng="pl">Mariusz</trd>
  </snc>
</drv>

</art>

