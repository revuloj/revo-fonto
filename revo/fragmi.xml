<vortaro>
<art mrk="$Id$">
<kap>
  <rad>fragmit</rad>/o
</kap>
<drv mrk="fragmi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Phragmites</trd>),
      kreskanta en mar&ccirc;oj kaj &ccirc;e lagoj kaj uzata ekzemple
      por fabriki matojn kaj tegi tegmentojn.
    </dif>
  </snc>
  <trd lng="de">Schilf</trd>
  <trd lng="ru">&c_t;&c_r;&c_o;&c_s;&c_t;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
