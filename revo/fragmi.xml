<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>fragmit</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fragmi.0o">
  <kap><tld/>o</kap>
  <snc mrk="fragmi.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="kan.0o.BOT">Kano</ref> vivanta en mar&ccirc;oj 
      kaj &ccirc;e lagoj, uzata por fari matojn, tegi tegmentojn...,
      genro el familio <ref tip="super" cel="poac.0oj">poacoj</ref> 
      (<trd lng="la">Phragmites</trd>):
      <ekz>
        &ccirc;u la <tld/>oj rekreskos en la venonta jaro? &Ccirc;u la ru&gcirc;vertaj 
        gruoj reflugos &ccirc;i tien?
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.espero.com.cn/08h/08h-03/ziran3-9.htm">
          &Ccirc;u ru&gcirc;vertaj gruoj...</url></vrk>,
          <lok>2004-11-15</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_y;&c_s;&c_mol;&c_n;&c_jo;&c_g;</trd>,
    <trd>&c_t;&c_r;&c_y;&c_s;&c_mol;&c_c;&c_jo;</trd>
  </trdgrp>
  <trd lng="de">Schilf</trd>
  <trd lng="fr">roseau<klr> (phragmite)</klr></trd>
  <trd lng="hu">n&aacute;d</trd>
  <trd lng="nl">riet</trd>
  <trd lng="pt">fragmente</trd>
  <trd lng="ru">&c_t;&c_r;&c_o;&c_s;&c_t;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
