<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fragmi.xml,v 1.24 2023/11/10 06:08:00 revo Exp $">
<kap>
  <rad>fragmit</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fragmi.0o">
  <kap><tld/>o</kap>
  <snc mrk="fragmi.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="kan.0o.BOT" lst="voko:pooparencoj">Kano</ref> 
      kreskanta en mar&ccirc;oj kaj &ccirc;e lagoj, uzata por fari matojn, 
      tegi tegmentojn ktp;
      <ref tip="lst" cel="genr.0o" lst="voko:botanikaj_genroj">genro</ref> 
      el familio <ref tip="malprt" cel="poac.0oj">poacoj</ref> 
      (<trd lng="la">Phragmites</trd>):
      <ekz>
        ili faros bongustajn bulojn el la nekuiritaj hordeaj grenoj, kaj metos la bulojn
        sur <tld/>ajn tigojn
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Du</lok>
        </fnt>;
      </ekz>
      <ekz>
        la mar&ccirc;oj prezentas mirigan mozaikon <klr>[&#x2026;]</klr> el <tld/>oj, kie akvo kaj
        &ccirc;ielo najbaras
        <fnt>
          <bib>Monato</bib>
          <aut>Jean-Yves Santerre</aut>
          <vrk><url ref="&Monato;/2013/010991.php">Kie akvo kaj &ccirc;ielo najbaras</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lo&gcirc;antoj scipovas profiti naturajn rimedojn,
        <klr>[&#x2026;]</klr> tegmentante siajn domojn per <tld/>a&jcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Jean-Yves Santerre</aut>
          <vrk><url ref="&Monato;/2013/010991.php">Kie akvo kaj &ccirc;ielo najbaras</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u la <tld/>oj rekreskos en la venonta jaro? &ccirc;u la ru&gcirc;vertaj 
        gruoj reflugos &ccirc;i tien?
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.espero.com.cn/08h/08h-03/ziran3-9.htm">
          &Ccirc;u ru&gcirc;vertaj gruoj...</url></vrk>,
          <lok>2004-11-15</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_y;&c_s;&c_mol;&c_n;&c_jo;&c_g;</trd>,
    <trd>&c_t;&c_r;&c_y;&c_s;&c_mol;&c_c;&c_jo;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>r&aacute;kos</trd>,
    <trd>t&rcaron;tina</trd>
  </trdgrp>
  <trd lng="de">Schilf</trd>
  <trd lng="fr">roseau<klr> (phragmite)</klr></trd>
  <trd lng="hu">n&aacute;d</trd>
  <trdgrp lng="ja">
    <trd>&#x30E8;&#x30B7;</trd>,
    <trd>&#x30A2;&#x30B7;</trd>
  </trdgrp>
  <trd lng="nl">riet</trd>
  <trd lng="pt">fragmente</trd>
  <trd lng="ru">&c_t;&c_r;&c_o;&c_s;&c_t;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">trs&tcaron; oby&ccaron;ajn&aacute;</trd>
  <trd lng="uk">&c_o;&c_ch;&c_je;&c_r;&c_je;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#x82A6;&#x7B1B; <pr>l&#xFA;d&#xED;</pr></trd>,
    <trd>&#x8305;&#x8349; <pr>m&#xE1;oc&#x1CE;o</pr></trd>,
    <trd>&#x9999;&#x84B2; <pr>xi&#x101;ngp&#xFA;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fragmi.0ejo">
  <kap>
    <tld/>ejo
  </kap>
  <snc>
    <dif>
      Loko, kie <tld/>oj abunde kreskas:
      <ekz>
        malprofundaj lagetoj kaj <tld/>ejoj akompane kun birdoj &ndash; vera spektaklo de la
        naturo
        <fnt>
          <bib>Monato</bib>
          <aut>Jean-Yves Santerre</aut>
          <vrk><url ref="&Monato;/2013/010991.php">Kie akvo kaj &ccirc;ielo najbaras</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> Granda bota&ubreve;ro <klr>[&#x2026;]</klr> dumtage ka&scirc;as sin en <tld/>ejoj,
        densaj mar&ccirc;oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=107940">Granda bota&ubreve;ro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/4/4f/Schwielowsee-Schilfrohrguertel-01.jpg/640px-Schwielowsee-Schilfrohrguertel-01.jpg" prm="PD"><tld/>ejo borde de lago Schwielow &ccirc;e rivero Havel, Brandenburgo, Germanujo
        <fnt>
          <aut>Botaurus stellaris, propra verko</aut>
          <lok><url ref="&WCW;/File:Schwielowsee-Schilfrohrguertel-01.jpg">&WCW;/File:Schwielowsee-Schilfrohrguertel-01.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_p;&c_n;&c_ja;&c_k;&c_ib;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_w;&c_n;&c_ib;</trd>
  </trdgrp>
  <trd lng="de">R&ouml;hricht</trd>
  <trd lng="en">reed bed</trd>
  <trd lng="fr">roseli&egrave;re</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&#x301;&c_v;&c_n;&c_i;</trd>
  <trd lng="uk">&c_p;&c_l;&c_a;&#x301;&c_v;&c_n;&c_ib;</trd>
  <trd lng="zh">&#x82A6;&#x82C7;&#x4E1B; <pr>l&#xFA;w&#x11B;ic&#xF3;ng</pr></trd>
</drv>





