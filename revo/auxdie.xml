<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: auxdie.xml,v 1.17 2023/04/10 18:22:01 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>a&ubreve;dienc</rad>/o
</kap>
<drv mrk="auxdie.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="akcept.0o.ago">Akcepto</ref> de oficialulo al vizitantoj:
      <ekz>
        ili petas <tld/>on &ccirc;e la re&gcirc;ino
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> ordonis al mi peti <tld/>on de via sankteco
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        peti <tld/>on de la ministro
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>;
      </ekz>
      <ekz>
        post la dua akto de la teatra&jcirc;o mi petis <tld/>on &ccirc;e la re&gcirc;isoro, kiu komence ne
        volis akcepti min
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la unua <tld/>o ne da&ubreve;ris pli ol du a&ubreve; tri minutojn, kaj &scirc;ajne ne estis tre
        kontentiga por &ccirc;iuj koncernatoj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; ol <nom>Melzar</nom> eliris el la <ind><tld/>ejo</ind>, la &ccirc;ambelano enlasis &jcirc;us alvenintan
        oficiron
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
        <trd lng="de">Audienzzimmer</trd>
      </ekz>
      <ekz>
        tie la reganto de la Babela imperio donis frumatenajn <tld/>ojn
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        disponeblas la notoj rilate la <tld/>ojn de Pio la 11a kun diplomatoj
        akredititaj &ccirc;e la Sankta Se&gcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Silvia Garnero</aut>
          <vrk><url ref="&Monato;/2006/009075.php"
              >Sekretoj studeblaj</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_w;&c_d;&c_y;&c_je;&c_n;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_jo;&c_m; <klr>(&c_a;&c_f;&c_ib;&c_c;&c_y;&c_j;&c_n;&c_y;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Audienz</trd>,
    <trd>offizieller <ind>Empfang</ind></trd>
  </trdgrp>
  <trd lng="en">audience</trd>
  <trd lng="es">audiencia</trd>
  <trd lng="fr">audience</trd>
  <trd lng="he">&resh;&alef;&yod;&vav;&fnun; &resh;&shin;&mem;&yod;</trd>
  <trdgrp lng="hu">
    <trd>audiencia</trd>,
    <trd>kihallgat&aacute;s</trd>
  </trdgrp>
  <trd lng="it">udienza<klr> (incontro)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_u;&c_d;&c_i;&c_je;&c_n;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_jo;&c_m; <klr>(&c_o;&c_f;&c_i;&c_c;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;)</klr></trd>
  </trdgrp>
  <trd lng="sk">audiencia</trd>
  <trd lng="sv">audiens</trd>
  <trdgrp lng="uk">
    <trd>&c_a;&c_u;&c_d;&c_ib;&c_jeu;&c_n;&c_c;&c_ib;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_j;&c_o;&c_m;&nbsp;(&c_o;&c_f;&c_ib;&c_c;&c_ib;&c_j;&c_n;&c_i;&c_j;)</trd>
  </trdgrp>
</drv>
</art>
