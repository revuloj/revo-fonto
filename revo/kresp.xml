<vortaro>
<art mrk="$Id: kresp.xml,v 1.26 2023/12/03 09:59:00 revo Exp $">
<kap>
  <rad>kresp</rad>/o
</kap>
<drv mrk="kresp.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Maldika pasta&jcirc;o farita el faruno miksita kun lakto kaj ovoj,
      kiun oni fritas en pato kaj man&gcirc;as kun sukerpulvoro;
      <ref tip="sin" cel="kuk.pat0o">patkuko</ref>:
      <ekz>
        la rizo <klr>[&#x2026;]</klr> uzata en &ccirc;iu man&gcirc;o <klr>[kiel]</klr> bovlo da blanka
        rizo, parfumita rizo, nudeloj, vermi&ccirc;eloj, <tld/>oj, alkohola&jcirc;o
        <fnt>
       <aut>Maryvonne kaj Bruno Robineau</aut>
          <vrk><url ref="&Monato;/2000/006831.php">La&ubreve; ritmo de la rizejoj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/02, p. 18</lok>
        </fnt>;
      </ekz>
      <ekz>
        rostado de kolbasetoj, fritado de krespoj &dash; ekstere &ccirc;e bivakfajro &dash; fari&gcirc;as
        furora&jcirc;oj de la vesperoj
        <fnt><aut>Saliko</aut>
        <vrk><url ref="&Monato;/2000/007162.html">&Gcirc;ojoj kaj
        &gcirc;uoj de finna somero</url></vrk>
        <bib>Monato</bib>
        <lok>2000/12, p. 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        eblis &gcirc;ui germanan bieron, estonajn mezepokajn dol&ccirc;a&jcirc;ojn, rusajn <tld/>ojn kun
        kaviaro
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2011/010405.php">Bruas la mezepoko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_l;&c_ib;&c_n;</trd>,
    <trd>&c_a;&c_l;&c_a;&c_d;&c_k;&c_a;</trd>
  </trdgrp>        
  <trd lng="br">krampouezhenn</trd>
  <trdgrp lng="de">
    <trd>Pfannkuchen</trd>
    <trd>Cr&ecirc;pe</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>crepe</trd>,
    <trd>pancake</trd>
  </trdgrp>
  <trd lng="fr">cr&ecirc;pe</trd>
  <trd lng="hu">palacsinta</trd>
  <trd lng="ja">&#x30AF;&#x30EC;&#x30FC;&#x30D7;</trd>
  <trdgrp lng="nl">
    <trd>pannenkoek</trd>,
    <trd>flensje</trd>
  </trdgrp>
  <trd lng="pl">nale&sacute;nik</trd>
  <trdgrp lng="pt">
    <trd>filh&oacute;</trd>,
    <trd>coscor&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_l;&c_i;&c_n;</trd>,
    <trd>&c_o;&c_l;&c_a;&c_d;&c_mol;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>pannkaka</trd>,
    <trd>cr&ecirc;pe</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_a;&#x43B;&c_i;&c_s;&#x43D;&c_i;&#x43A;</trd>,
    <trd>&#x43C;&#x43B;&c_i;&#x43D;&c_je;&c_c;&#x44C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8584;&#x70E4;&#x9905; <pr>b&#xF3;k&#x1CE;ob&#x1D0;ng</pr></trd>,
    <trd>&#x8584;&#x70E4;&#x997C; <pr>b&#xF3;k&#x1CE;ob&#x1D0;ng</pr></trd>,
    <trd>&#x8584;&#x9905;&#x6372; <pr>b&#xE1;ob&#x1D0;ngju&#x1CE;n</pr></trd>,
    <trd>&#x8584;&#x997C;&#x5377; <pr>b&#xE1;ob&#x1D0;ngju&#x1CE;n</pr></trd>
  </trdgrp>
</drv>
</art>
