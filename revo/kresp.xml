<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kresp.xml,v 1.20 2019/06/30 18:21:03 revo Exp $">
<kap>
  <rad>kresp</rad>/o
</kap>
<drv mrk="kresp.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Maldika pasta&jcirc;o farita el faruno miksita kun lakto kaj ovoj,
      kiun oni fritas en pato kaj man&gcirc;as kun sukerpulvoro;
      <ref tip="sin" cel="kuk.pat0o">patkuko</ref>:
      <ekz>
        la rizo <klr>[&#8230;]</klr> uzata en &ccirc;iu man&gcirc;o <klr>[kiel]</klr> bovlo da blanka
        rizo, parfumita rizo, nudeloj, vermi&ccirc;eloj, <tld/>oj, alkohola&jcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>S. Maul</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006831.html"
              >La&ubreve; ritmo de la rizejoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        rostado de kolbasetoj, fritado de krespoj &dash; ekstere &ccirc;e bivakfajro &dash; fari&gcirc;as
        furora&jcirc;oj de la vesperoj
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007162.html"
              >&Gcirc;ojoj kaj &gcirc;uoj de finna somero</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        eblis &gcirc;ui germanan bieron, estonajn mezepokajn dol&ccirc;a&jcirc;ojn, rusajn <tld/>ojn kun
        kaviaro
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2011/010405.php"
              >Bruas la mezepoko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_l;&c_ib;&c_n;</trd>,
    <trd>&c_a;&c_l;&c_a;&c_d;&c_k;&c_a;</trd>
  </trdgrp>        
  <trd lng="br">krampouezhenn</trd>
  <trdgrp lng="de">
    <trd>Pfannkuchen</trd>
    <trd>Cr&ecirc;pe</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>crepe</trd>,
    <trd>pancake</trd>
  </trdgrp>
  <trd lng="fr">cr&ecirc;pe</trd>
  <trd lng="hu">palacsinta</trd>
  <trdgrp lng="nl">
    <trd>pannenkoek</trd>,
    <trd>flensje</trd>
  </trdgrp>
  <trd lng="pl">nale&sacute;nik</trd>
  <trdgrp lng="pt">
    <trd>filh&oacute;</trd>,
    <trd>coscor&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_l;&c_i;&c_n;</trd>,
    <trd>&c_o;&c_l;&c_a;&c_d;&c_mol;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>pannkaka</trd>,
    <trd>cr&ecirc;pe</trd>
  </trdgrp>
</drv>
</art>
