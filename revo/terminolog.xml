<art mrk="$Id: terminolog.xml,v 1.11 2023/08/09 07:24:57 revo Exp $">
<kap>
  <rad>terminolog</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="terminolog.0o">
  <kap><tld/>o</kap>
  <snc mrk="terminolog.0o.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="dif" cel="termin.0aristo.LIN">Terminaristo</ref>:
      <ekz>
        se Esperanto iam havos ta&ubreve;gan kaj belan terminaron,
        &gcirc;in kreos ne la fu&scirc;uloj, kiuj sin nomas
        <tld/>oj, sed niaj beletristoj
        <fnt>
          <aut>E. Grimley-Evans</aut>,
          <vrk>Lingvaj Notetoj 1992:3</vrk>,
          <lok>soc.culture.esperanto, 1992-05-06</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Eugen W&uuml;ster</nom> <klr>[&#8230;]</klr> estis a&ubreve;stra industriisto, <tld/>o kaj fondinto de
        la moderna terminologia fako
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1119">Eugen W&uuml;ster</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_k;&c_l;&c_a;&c_d;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k; &c_t;&c_e;&c_r;&c_m;&c_ib;&c_n;&c_a;&c_l;&c_a;&c_g;&c_ib;&c_ch;&c_n;&c_a;&c_g;&c_a; &c_s;&c_l;&c_o;&c_w;&c_n;&c_ib;&c_k;&c_a;</trd>
  <trd lng="ca">termin&ograve;leg</trd>
  <trdgrp lng="cs">
    <trd>odborn&iacute;k v terminologii</trd>,
    <trd>terminolog</trd>
  </trdgrp>
  <trd lng="de">Terminologe</trd>
  <trd lng="fr">terminologue</trd>
  <trd lng="it">terminologista</trd>
  <trd lng="pt">terminologista</trd>
  <trd lng="ru">&c_t;&c_je;&c_r;&c_m;&c_i;&c_n;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="sk">terminol&oacute;g</trd>
  <trd lng="uk">&c_f;&c_a;&c_h;&c_ib;&c_v;&c_je;&c_c;&#x44C; &c_z; &c_t;&c_je;&c_r;&#x43C;&c_ib;&#x43D;&#x43E;&#x43B;&#x43E;&c_g;&c_ib;&c_ji;</trd>
</drv>
</art>

