<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: miken.xml,v 1.1 2023/07/18 18:30:54 afido Exp $">
    <kap>
      <rad>Miken</rad>/o,
      <var><kap><rad var="c">Micen</rad>/o</kap></var>
    </kap>

<drv mrk="miken.0o">
  <kap><tld/>o, <var><kap><tld var="c"/>o</kap></var></kap>
  <snc mrk="miken.0o.urbo">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Grava greka <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref> de la bronzepoko,
      sur Peloponezo, &ccirc;. 80&#8239;km okcidente de Ateno:
      <ekz>
        al Sparto! al <tld/>o! al Delfoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>!
      </ekz>
      <ekz>
        en vestoj de fremduloj, ili venis <tld/>on
        <fnt>
          <bib>Ifigenio</bib>
          <lok>Ifigenio en Ta&ubreve;rido</lok>
        </fnt>;
      </ekz>
      <ekz>
        post la falo de la <tld lit="m" var="c"/>aj centroj fine de la 13a jarcento a.K. <klr>[&#8230;]</klr>
        multaj gentoj translo&gcirc;i&gcirc;is de &ccirc;e Egea Maro al apudaj regionoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.be/2014/011357.php"
              >Kio estas la kipra silabaro?</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Mykene</trd>
    <trd lng="en">Mycenae</trd>
    <trd lng="fr">Myc&egrave;nes</trd>
  <trd lng="grc">&My;&ypsilon;&kappa;&eta_ton;&ny;&eta;</trd>
  <trd lng="ru">&c_M;&c_i;&c_k;&c_je;&c_n;&c_y;</trd>
  </snc>
</drv>

  </art>

