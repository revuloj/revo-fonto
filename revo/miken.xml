<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: miken.xml,v 1.5 2024/01/26 17:40:06 afido Exp $">
    <kap>
      <rad>Miken</rad>/o,
      <var><kap><rad var="c">Micen</rad>/o</kap></var>
    </kap>

<drv mrk="miken.0o">
  <kap><tld/>o, <var><kap><tld var="c"/>o</kap></var></kap>
  <snc mrk="miken.0o.urbo">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Grava greka <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref> de la bronzepoko,
      sur Peloponezo, &ccirc;. 80&#x202F;km okcidente de Ateno:
      <ekz>
        al Sparto! al <tld/>o! al Delfoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>!
      </ekz>
      <ekz>
        en vestoj de fremduloj, ili venis <tld/>on
        <fnt>
          <bib>Ifigenio</bib>
          <lok>Ifigenio en Ta&ubreve;rido</lok>
        </fnt>;
      </ekz>
      <ekz>
        post la falo de la <tld lit="m" var="c"/>aj centroj fine de la 13a jarcento a.K.
        <klr>[&#x2026;]</klr>
        multaj gentoj translo&gcirc;i&gcirc;is de &ccirc;e Egea Maro al apudaj regionoj
        <fnt>
        <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011357.php">Kio estas la kipra silabaro?</url></vrk>
               <bib>Monato</bib>
          <lok>2014/10, p. 16</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_M;&c_ib;&c_k;&c_je;&c_n;&c_y;</trd>
    <trd lng="de">Mykene</trd>
    <trd lng="en">Mycenae</trd>
    <trd lng="fr">Myc&egrave;nes</trd>
  <trd lng="grc">&My;&ypsilon;&kappa;&eta_ton;&ny;&eta;</trd>
  <trdgrp lng="nl">
    <trd>Mycene</trd>,
    <trd>Mykene</trd>
  </trdgrp>
  <trd lng="ru">&c_M;&c_i;&c_k;&c_je;&c_n;&c_y;</trd>
    <trdgrp lng="zh">
    <trd>&#x8FC8;&#x9521;&#x5C3C; <pr>m&#xE0;ix&#xED;n&#xED;</pr></trd>,
    <trd>&#x9081;&#x932B;&#x5C3C; <pr>m&#xE0;ix&#xED;n&#xED;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="ja">
    <trd>&#x30DF;&#x30B1;&#x30FC;&#x30CD;</trd>,
    <trd>&#x30DF;&#x30E5;&#x30B1;&#x30CA;&#x30A4;</trd>
  </trdgrp>
</drv>

  </art>

