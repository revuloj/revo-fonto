<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>4</ofc>
<rad>fikci</rad>/o
</kap>
<drv mrk="fikci.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Arbitra krea&jcirc;o de la fantazio:
      <ekz>poezia <tld/>o;</ekz>
      <ekz>fabelaj <tld/>oj.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="mit.0o">mito</ref>,
      <ref cel="fabel">fabelo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_f;&c_i;&c_k;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_v;&c_y;&c_m;&c_y;&c_s;&c_je;&c_l;</trd>
  </trdgrp>
</drv>
<drv mrk="fikci.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Imagita, nereala, nur &scirc;ajna, konvencia:
      <ekz><tld/>a rakonto, heroo;</ekz>
      <ekz><tld/>a bilanco;</ekz>
      <ekz>bankbiletoj estos nur <tld/>a mono;</ekz>
      <ekz>komitato havanta forton ne <tld/>an sed efektivan<fnt>Z</fnt>.
      </ekz>  
    </dif>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_f;&c_i;&c_k;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_y;&c_m;&c_y;&c_sh;&c_l;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_m;&c_n;&c_i;&c_m;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
<!--
$Log$
-->
</vortaro>
