<drv mrk="fikci.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Arbitra <ref tip="super" cel="kre.0ajxo.KOMUNE">krea&jcirc;o</ref>
      de la <ref tip="super" cel="fantaz.0o.fantaziajxo">fantazio</ref>:
      <ekz>
        nova &gcirc;enro <klr>[estas la]</klr> scienca <tld/>o <klr>[de]</klr> <nom>J. Verne</nom>
        <fnt>
          <bib>KFl</bib>
        </fnt>;
      </ekz>
      <ekz>
        jurista <tld/>o
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ekzistas respektinda esperanta <tld/>aro
        <fnt>
          <bib>Plumamikoj</bib>
        </fnt>;
      </ekz>
      <ekz>
        la plej interesa <tld/>o de <nom>Baghy</nom> estas surprize la avangarda romano
        <fnt>
          <bib>Plumamikoj</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pens.el0ajxo">elpensa&jcirc;o</ref>,
      <ref cel="fabel">fabelo</ref>,
      <ref cel="fantast.0o">fantasto</ref>,
      <ref cel="mit.0o">mito</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_ib;&c_k;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_v;&c_y;&c_d;&c_u;&c_m;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">ficci&oacute;</trd>
  <trdgrp lng="cs">
    <trd>fikce</trd>,
    <trd>smy&scaron;lenka</trd>,
    <trd>zd&aacute;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Dichtung</trd>,
    <trd>Fiktion</trd>
  </trdgrp>
  <trd lng="en">fiction</trd>
  <trd lng="es">ficci&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>fiction</trd>,
    <trd>invention<klr tip="ind"> (fiction)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fikci&oacute;</trd>,
    <trd>kital&aacute;l&aacute;s</trd>,
    <trd>elmesz&uuml;lem&eacute;ny</trd>
  </trdgrp>
  <trd lng="it">finzione</trd>
  <trdgrp lng="ja">
    <trd>&#x865A;&#x69CB;  <pr>&#x304D;&#12423;&#12371;&#12358;</pr></trd>,
    <trd>&#x30D5;&#x30A3;&#x30AF;&#x30B7;&#x30E7;&#x30F3;  <pr>&#12405;&#12355;&#x304F;&#12375;&#12423;&#12435;</pr></trd>,
    <trd>&#x4F5C;&#x308A;&#x4E8B;  <pr>&#12388;&#x304F;&#x308A;&#12372;&#12392;</pr></trd>,
    <trd>&#x4F5C;&#x308A;&#x8A71;  <pr>&#12388;&#x304F;&#x308A;&#12400;&#x306A;&#12375;</pr></trd>
  </trdgrp>
  <trd lng="nl">fictie</trd>
  <trd lng="pl">fikcja</trd>
  <trd lng="pt">fic&ccedil;&atilde;o</trd>
  <trd lng="ro">fic&#x21B;iune</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_i;&c_k;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_v;&c_y;&c_m;&c_y;&c_s;&c_je;&c_l;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>fikcia</trd>,
    <trd>v&yacute;mysel</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_f;&c_ib;&#x43A;&c_c;&c_ib;&#x44F;</trd>,
    <trd>&c_v;&c_i;&#x43C;&c_i;&c_s;&c_je;&#x43B;</trd>,
    <trd>&c_f;&c_a;&#x43D;&c_t;&c_a;&c_s;&c_t;&c_i;&#x43A;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5B50;&#x865A; <pr>z&#x1D0;x&#x16B;</pr></trd>,
    <trd>&#x5B50;&#x865A;&#x4E4C;&#26377; <pr>z&#x1D0;x&#x16B;w&#x16B;y&#x1D2;u</pr></trd>,
    <trd>&#x865A;&#26500;&#x5C0F;&#x8BF4; <pr>x&#x16B;g&#xF2;uxi&#x1CE;oshu&#x14D;</pr></trd>,
    <trd>&#x675C;&#x64B0; <pr>d&#xF9;zhu&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fikci.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Imagita, <ref tip="dif" cel="real.ne0a">nereala</ref>,
      nur <ref tip="sin" cel="sxajn.0a">&scirc;ajna</ref>:
      <ekz>
        komitato, kiu havas forton ne <tld/>an sed efektivan
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista en Washington en la 15a de a&ubreve;gusto
          1910</vrk>
        </fnt>;
      </ekz>
      <ekz>
        se la reklaminto enketos, li malkovros ke la adresoj estas <tld/>aj, sed se li
        ne enketos, mi eble havos laboron
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        liaj temoj estas <tld/>aj, kaj ne trovas korespondon en la reala&jcirc;o
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ia simileco kun <tld/>aj personoj, elpensitaj de similnoma verkisto, estas
        plene intenca
        <fnt>
          <bib>ChM</bib>
        </fnt>;
      </ekz>
      <ekz>
        bonega enkonduko en la <tld/>an prozon esperantan estas la
        antologio 33 rakontoj (1964), redaktita de <nom>Reto Rossetti</nom> kaj
        <nom>Ferenc Szil&aacute;gyi</nom>
        <fnt><bib>Plumamikoj</bib>, <lok>prozo fikcia</lok></fnt>;
      </ekz>
      <ekz>
        &gcirc;ia sola informfonto estas romanoj kaj filmoj &ndash; do puraj <tld/>a&jcirc;oj
        <fnt>
          <bib>CKv</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fantaz.0a.malreala">fantazia<sncref/></ref>,
      <ref cel="iluzi.0a">iluzia</ref>,
      <ref cel="hximer.0o.FIG">&hcirc;imero<sncref/></ref>,
      <ref cel="konven1.0a">konvencia</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_ib;&c_k;&c_c;&c_y;&c_j;&c_n;&c_y;</trd>,
    <trd>&c_f;&c_ib;&c_k;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_v;&c_y;&c_d;&c_u;&c_m;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_u;&c_ja;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">fictici</trd>
  <trdgrp lng="cs">
    <trd>fiktivn&iacute;</trd>,
    <trd>smy&scaron;len&yacute;</trd>,
    <trd>zd&aacute;nliv&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>erdichtet</trd>,
    <trd>fiktiv</trd>,
    <trd>fingiert</trd>,
    <trd>angenommen <klr>(fiktiv)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>fictional</trd>,
    <trd>fictitious</trd>
  </trdgrp>
  <trd lng="es">ficticio</trd>
  <trdgrp lng="fr">
    <trd>fictif</trd>,
    <trd>imaginaire</trd>,
    <trd>invent&eacute;<klr tip="ind"> (fictif)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fikt&iacute;v</trd>,
    <trd>kital&aacute;lt</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>fittizio</trd>,
    <trd>immaginario</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x67B6;&#x7A7A;&#x306E;  <pr>&#x304B;&#x304F;&#12358;&#x306E;</pr></trd>,
    <trd>&#x865A;&#x69CB;&#x306E;  <pr>&#x304D;&#12423;&#12371;&#12358;&#x306E;</pr></trd>,
    <trd>&#x898B;&#x305B;&#x304B;&#12369;&#x306E;  <pr>&#x307F;&#x305B;&#x304B;&#12369;&#x306E;</pr></trd>,
    <trd>&#x507D;&#x308A;&#x306E;  <pr>&#12356;&#12388;&#x308F;&#x308A;&#x306E;</pr></trd>,
    <trd>&#x30D5;&#x30A3;&#x30AF;&#x30B7;&#x30E7;&#x30F3;&#x306E;
    <pr>&#12405;&#12355;&#x304F;&#12375;&#12423;&#12435;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">fictief</trd>
  <trdgrp lng="pl">
    <trd>fikcyjny</trd>,
    <trd>nierealny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_f;&c_i;&c_k;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_y;&c_m;&c_y;&c_sh;&c_l;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_m;&c_n;&c_i;&c_m;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>fikt&iacute;vny</trd>,
    <trd>neskuto&ccaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_f;&c_ib;&#x43A;&c_t;&c_i;&c_v;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43C;&#x43D;&c_i;&#x43C;&c_i;&c_j;</trd>,
    <trd>&c_u;&#x44F;&c_v;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_i;&c_g;&c_a;&c_d;&c_a;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_f;&c_a;&#x43D;&c_t;&c_a;&c_s;&c_t;&c_i;&c_ch;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="zh">&#x865A;&#26500; <pr>x&#x16B;g&#xF2;u</pr></trd>
</drv>

<drv mrk="fikci.scienc0o">
  <kap>scienc<tld/>o</kap>
  <snc mrk="fikci.scienc0o.BELE">
    <uzo tip="fak">BELE</uzo>
    <uzo tip="fak">KIN</uzo>
    <dif>
      Rakonta <ref tip="super" cel="gxenr.0o">&gcirc;enro</ref>, kiu prezentas
      imaga&jcirc;ojn pri estontaj sekvoj de scienco kaj te&hcirc;niko, pri aliaj
      planedoj ol la tero ks temoj:
      <ekz>
        &leftquot;Hura!&rightquot; <klr>[&#8230;]</klr> miksas elementojn de filozofieca satiro, gajege
        ekstravaganca farsa satiro, scienc<tld/>o kaj fantazio
        <fnt>
          <bib>Plumamikoj</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiom da mal&gcirc;ustaj ideoj vi &ccirc;iuj havas! vi legis tro da t.n. scienc<tld/>aj verkoj
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i tie ne temas pri scienc<tld/>o sed pri <ref cel="fantast.0o">fantasto</ref>,
        kiu iom memorigas pri la  groteskaj pentra&jcirc;oj de <nom>Hieronymus Bosch</nom>
        <fnt>

          <aut>Garvan Makaj'</aut>
          <vrk><url ref="&Monato;/2008/009457.php">Nigra apokalipso</url></vrk>
          <bib>Monato</bib>
          <lok>2008/07, p. 22</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_v;&c_u;&c_k;&c_o;&c_v;&c_a;&c_ja; <ind>&c_f;&c_a;&c_n;&c_t;&c_a;&c_s;&c_t;&c_y;&c_k;&c_a;</ind></trd>
  <trd lng="ca">ci&egrave;ncia-ficci&oacute;</trd>
  <trd lng="de">Science Fiction</trd>
  <trd lng="en">science fiction</trd>
  <trd lng="es">ciencia ficci&oacute;n</trd>
  <trd lng="fr">science-fiction</trd>
  <trd lng="it">fantascienza</trd>
  <trdgrp lng="ja">
    <trd>&#x7A7A;&#x60F3;&#x79D1;&#x5B66;&#x5C0F;&#x8AAC;
    <pr>&#x304F;&#12358;&#x305D;&#12358;&#x304B;&#x304C;&#x304F;&#12375;&#12423;&#12358;&#x305B;&#12388;</pr></trd>,
    <trd>&#x30B5;&#x30A4;&#x30A8;&#x30F3;&#x30B9;&#x30D5;&#x30A3;&#x30AF;&#x30B7;&#x30E7;&#x30F3;</trd>
  </trdgrp>
  <trd lng="nl">science fiction</trd>
  <trd lng="pl">fantastyka naukowa</trd>
  <trd lng="ro">&#537;tiin&#x21B;&abreve;-fic&#x21B;iune</trd>
  <trd lng="ru">&c_n;&c_a;&c_u;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_f;&c_a;&c_n;&c_t;&c_a;&c_s;&c_t;&c_i;&c_k;&c_a;</ind></trd>
  <trd lng="uk">&#x43D;&c_a;&c_u;&#x43A;&#x43E;&c_v;&c_a; &c_f;&c_a;&#x43D;&c_t;&c_a;&c_s;&c_t;&c_i;&#x43A;&c_a;</trd>
  <trd lng="zh">&#x79D1;&#x5E7B; <pr>k&emacron;hu&#xE0;n</pr></trd>
</drv>

<drv mrk="fikci.terur0o">
  <kap>terur<tld/>o,
    <var><kap>horora <tld/>o</kap></var>
  </kap>
  <snc>
    <uzo tip="fak">BELE</uzo>
    <uzo tip="fak">KIN</uzo>
    <dif>
      Rankonta <ref tip="super" cel="gxenr.0o">&gcirc;enro</ref>, kiu amuzcele timigas, nervozigas, a&ubreve;
      alimaniere maltrankviligas sian publikon:
      <ekz>
        horora <tld/>o, terur<tld/>o, a&ubreve; fojfoje simple hororo, estas &gcirc;enro de <tld/>o, kiu celas
        ka&ubreve;zi timigan, maltrankviligan a&ubreve; nervozigan etoson
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=126273"
              >Horora fikcio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="horor.0o.BELE">hororo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_a;&c_n;&c_r; <ind>&c_zh;&c_a;&c_h;&c_a;&c_w;</ind></trd>,
    <trd>&c_h;&c_o;&c_r;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="de">Horror</trd>
  <trd lng="en">horror<klr tip="ind"> (genre)</klr></trd>
  <trd lng="fr">horreur<klr tip="ind"> (genre)</klr></trd>
  <trd lng="ja">&#12507;&#12521;&#12540;</trd>
  <trd lng="nl">horror</trd>
</drv>
</art>
