<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hind</rad>/o
</kap>
<drv mrk="hind.0o">
  <kap><tld/>o</kap>
  <snc mrk="hind.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="hind.H0ujo"><tld lit="H"/>ujo</ref>.
    </dif>
    <trd lng="de">Indischer Staatsb&uuml;rger</trd>
    <trd lng="hu">indiai (&aacute;llampolg&aacute;r)</trd>
  </snc>
  <snc mrk="hind.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco:
      <ekz>
        fakte &ccirc;inoj kaj <tld/>oj <klr>(en Malajzio)</klr> scipovas
        paroli sian gepatran lingvon, sed estas analfabetaj en tiu
        lingvo
        <fnt><bib>Monato</bib>, <lok>n-ro 02/1994, Malajzio &dash;
        Multlingveco</lok></fnt>;
      </ekz>
      <ekz>
        en sia longa ru&gcirc;a vesto staras la <tld lit="H"/>ino sur
        la brullignaro, avide la flamoj el&scirc;ovas siajn langojn
        supren
        &ccirc;irka&ubreve; la virino kaj &scirc;ia mortinta edzo
        <fnt><bib>Fab2</bib>, <lok>Ne&gcirc;a re&gcirc;ino</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Inder</trd>
    <trd lng="hu">hindu (nemzetis&eacute;g&udblac;)</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>indien<klr tip="ind"> (habitant de l'Inde)</klr></trd>
  </trdgrp>
  <trd lng="nl">Indi&euml;r</trd>
  <trd lng="pl">Hindus</trd>
  <trd lng="ru">&c_i;&c_n;&c_d;&c_i;&c_je;&c_c;</trd>
</drv>

<drv mrk="hind.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilantanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato:
      <ekz>
        sur la folio de la lotuso &gcirc;i pelas la sanktan akvon de
        la rivero Gango kaj la okuloj de la <tld lit="H"/>a knabino
        eklumas,
        kiam &scirc;i &gcirc;in ekvidas
        <fnt><bib>Fab3</bib>, <lok>Birdo Fenikso</lok></fnt>;
      </ekz>
      <ekz>
        <klr>(en Malajzio)</klr> la dua oficiala lingvo estas la angla
        kaj aliaj gravaj lingvoj estas la mandarena (&ccirc;ina)
        kaj tamila (<tld/>a), en hindaj kaj &ccirc;inaj familioj
        la infanoj parolas hinde a&ubreve; &ccirc;ine kaj en lernejoj
        ili lernas la malajan kaj anglan
        <fnt><bib>Monato</bib>, <lok>n-ro 02/1994, Malajzio &dash;
        Multlingveco</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">indisch</trd>
  <trdgrp lng="fr">
    <trd>indien<klr tip="ind"> (adj., d'Inde)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hindu (nemzetis&eacute;g&udblac;)</trd>,
    <trd></trd>indiai
  </trdgrp>
  <trd lng="nl">Indisch</trd>
  <trd lng="pl">indyjski</trd>
  <trd lng="ru">&c_i;&c_n;&c_d;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="hind.H0ujo">
  <kap><tld lit="H"/>ujo</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="sxtat.0o">&Scirc;tato</ref> situanta
      en suda <ref tip="malprt" cel="azi.A0o">Azio</ref>
      kaj limanta norde al Pakistano, &Ccirc;inujo, Nepalo kaj
      sude kiel dento etendi&gcirc;as en la <tld lit="H"/>an Oceanon:
      <ekz>
        tio estis en la tempo de A&hcirc;a&scirc;vero&scirc;, kiu
        re&gcirc;is
        de <tld lit="H"/>ujo &gcirc;is Etiopujo super cent dudek sep
        landoj
        <fnt><bib>MT</bib>, <lok>&Est; 1:1</lok></fnt>;
      </ekz>
      <ekz>
        Holger la Dano militakiris la grandan landon <tld lit="H"/>ujon
        &gcirc;is la orienta fino de la mondo, &gcirc;is tiu arbo, kiu
        estas nomata arbo de la suno
        <fnt><bib>Fab3</bib>, <lok>&Scirc;tono de la
        sa&gcirc;uloj</lok></fnt>;
      </ekz>
      <ekz>
        la ideo de Kolumbo ke devas ekzisti okcidenta vojo <tld
        lit="H"/>ujon
        &scirc;ajnas al ni nun tiel simpla, tiel natura
        <fnt><bib>EE</bib>, <lok>&ccirc;ap. 1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="sin" cel="barat.B0o">Barato</ref>
  <trd lng="de">Indien</trd>
  <trdgrp lng="fr">
    <trd>Inde</trd>
  </trdgrp>
  <trd lng="hu">India</trd>
  <trd lng="nl">Indi&euml;</trd>
  <trd lng="pl">India</trd>
  <trd lng="ru">&c_I;&c_n;&c_d;&c_i;&c_ja;</trd>
</drv>

<drv mrk="hind.H0aOceano">
  <kap><tld lit="H"/>a Oceano</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Parto de la <ref tip="malprt" cel="ocean.0o">oceano</ref>
      inter Afriko, Azio kaj Oceanio.
    </dif>
  </snc>
  <trd lng="de">Indischer Ozean</trd>
  <trdgrp lng="fr">
    <trd>Oc&eacute;an Indien</trd>
  </trdgrp>
  <trd lng="hu">Indiai-&oacute;ce&aacute;n</trd>
  <trd lng="nl">Indische oceaan</trd>
  <trd lng="pl">Ocean Indyjski</trd>
  <trd lng="ru">&c_I;&c_n;&c_d;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j;
  &c_o;&c_k;&c_je;&c_a;&c_n;</trd>
</drv>

<drv mrk="hind.0euxropa">
  <kap><tld/>e&ubreve;ropa</kap>
  <uzo tip="fak">LIN</uzo>
  <snc>
    <dif>
      Rilata al la granda lingvofamilio, disvasti&gcirc;inta en 15-10aj jc
      a.K.
      en <tld lit="H"/>ujo, Persujo, Anatolio kaj E&ubreve;ropo:
      <ekz>
        klopodon rekonstrui lingvan praformon (same kiel oni
        rekonstruis la <tld/>e&ubreve;ropan pralingvon) oni povas opinii
        a&ubreve; esprimo de estimo por la lingvaj tradicioj, a&ubreve;
        lingvistika ludismo
        <fnt><aut>Vera Barandovsk&aacute;-Frank</aut>:
        <lok><url
        ref="http://www.uni-paderborn.de/extern/fb/2/Kyb.Paed/INT/07-int.htm">Enkonduka
        lernolibro de interlingvistiko</url></lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">indogermanisch</trd>
  <trd lng="en">Indo-European</trd>
  <trdgrp lng="fr">
    <trd>indo-europ&eacute;en</trd>
  </trdgrp>
  <trd lng="nl">Indo-Europees</trd>
  <trd lng="pl">indo-europejski</trd>
  <trd lng="hu">indoeur&oacute;pai</trd>
  <trd lng="ru">&c_i;&c_n;&c_d;&c_o;&c_je;&c_v;&c_r;&c_o;&c_p;&c_je;&c_j;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
