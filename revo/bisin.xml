<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bisin.xml,v 1.6 2023/12/11 09:27:35 revo Exp $">
<kap>
  <rad>bisin</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="bisin.0o">
  <kap><tld/>o</kap>
  <snc mrk="bisin.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Antikva luksa <ref tip="super" cel="sxtof.0o.TEKS">&scirc;tofo</ref>,
    </dif>
    <subsnc>
      <dif>
        silkeca, el la sekreto de duvalvuloj, i.a. <nom>Pinna nobilis</nom>.
      </dif>
    </subsnc>
    <subsnc>
      <dif>
        el plantaj fibroj de lino k.a.:
        <ekz>
          li vestis lin per <tld/>aj vestoj kaj metis oran &ccirc;enon sur lian kolon
          <fnt>
            <bib>MT</bib>
            <lok>Genezo 41:42</lok>
          </fnt>;
        </ekz>
        <ekz>
          estis unu ri&ccirc;ulo, kiu estis vestita per purpuro kaj <tld/>o,
          kaj festis lukse &ccirc;iutage
          <fnt><bib>NT</bib>, <lok>&Luk; 16:19</lok></fnt>.
        </ekz>
      </dif>
    </subsnc>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_s;&c_o;&c_n;</trd>
  <trd lng="de">Byssus</trd>
  <trd lng="el">&beta;&ypsilon_ton;&sigma;&sigma;&omikron;&sigma_fina;</trd>
  <trd lng="fr">byssos</trd>
  <trd lng="pl">bisior</trd>
  <trd lng="ru">&c_v;&c_i;&c_s;&c_s;&c_o;&c_n;</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_a;&c_v;&c_l;&c_i;&c_k;&c_o;&c_v;&c_i;&c_j;/&c_ch;&c_je;&c_r;&c_je;&c_p;&c_a;&c_sh;&c_k;&c_o;&c_v;&c_i;&c_j; &c_sh;&c_o;&c_v;&c_k;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_s;&c_mol;&c_k;&c_i;&c_j; &c_sh;&c_o;&c_v;&c_k;</trd>
  </trdgrp>
</drv>
</art>

