<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>titan</rad>/o
</kap>
<drv mrk="titan.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      En greka mitologio, unu el gigantoj, kiuj provis invadi Olimpon.
    </dif>
  </snc>
  <trd lng="de">Titan</trd>
  <trd lng="nl">titan</trd>
  <trd lng="ru">&c_t;&c_i;&c_t;&c_a;&c_n; <klr tip="amb">(&c_g;&c_i;&c_g;&c_a;&c_n;&c_t;)</klr></trd>
</drv>

<drv mrk="titan.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Kolosa:
      <ekz>
        <tld/>a laboro.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">titanisch</trd>
  <trd lng="ru">&c_t;&c_i;&c_t;&c_a;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
