<drv mrk="region.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Koncernanta nur unu <tld/>on:
      <ekz>
        <tld/>a dialekto, kutimo, gazeto, ekspozicio;
      </ekz>
      <ekz>
        servado al <tld/>a komunumo
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a elekto
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9405-britio.txt">Britio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en Hispanio la socialistoj ne povas regi sen a&ubreve; kontra&ubreve; <tld/>aj partioj, kiuj
        akiris grandan apogon de elektantoj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9308-alarmsignalo.txt">El mia vidpunkto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ali<tld/>a leganto
        <fnt>
          <bib>Monato</bib>
          <aut>Probal Da&scirc;gupto</aut>
          <vrk><url ref="https://www.monato.net/2003/007815.html">Verki unu por la aliaj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi a&ubreve;das la popolan konversacian irlandan a&ubreve; iun <tld/>an dialekton
        <fnt>
          <bib>Monato</bib>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006880.html">Ne &ccirc;iuj lingvoj estas langaj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="provin.0a">provinca</ref>
    </refgrp>
  </snc>
  <trd lng="be">
  &c_r;&c_e;&c_g;&c_ib;&c_ja;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;
  </trd>
  <trdgrp lng="cs">
    <trd>krajov&yacute;</trd>,
    <trd>krajsk&yacute;</trd>,
    <trd>oblastn&iacute;</trd>,
    <trd>region&aacute;ln&iacute;</trd>,
    <trd>&uacute;zemn&iacute;</trd>
  </trdgrp>
  <trd lng="de">regional</trd>
  <trd lng="es">regional</trd>
  <trd lng="fr">r&eacute;gional</trd>
  <trdgrp lng="hu">
    <trd>t&eacute;rs&eacute;gi</trd>,
    <trd>region&aacute;lis</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>regionaal</trd>,
    <trd>streek-</trd>
  </trdgrp>
  <trdgrp lng="pl">
      <trd>regionalny</trd>,
      <trd>rejonowy</trd>,
      <trd>okoliczny</trd>,
      <trd>chor&aogonek;gwiany<klr> (w harcerstwie)</klr></trd>
    </trdgrp>
