    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_r;&c_je;&c_g;&c_i;&c_o;&c_n;</trd>,
      <trd>&c_o;&c_b;&c_l;&c_a;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_k;&c_r;&c_a;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="region.0o.organik">
    <dif>
      Parto da organika korpo karakterizata de specialaj ecoj:
      <ekz>
        lumbo &mdash; <tld/>o de la korpo, kie fini&gcirc;as la dorso kaj komenci&gcirc;as la femuroj
        <fnt>
          <bib>LR</bib>
          <lok>2. Vortuzo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_o;&c_b;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;
    </trd>
    <trdgrp lng="de">
      <trd>Gegend</trd>,
      <trd>K&ouml;rperregion</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>t&aacute;j</trd>,
      <trd>t&aacute;j&eacute;k</trd>
    </trdgrp>
    <trd lng="nl">streek</trd>
    <trdgrp lng="pl">
      <trd>rejon</trd>,
      <trd>okolica</trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_b;&c_l;&c_a;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="region.0o.fig">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="sin" cel="sfer.0o.FIG">Sfero</ref>, kampo de efiko de aferoj prezentanta ian specialan econ:
      <ekz>
        la <tld/>o de la spirito
        <fnt>
          <bib>Fab3</bib>
          <lok>Io</lok>
        </fnt>;
      </ekz>
      <ekz>
        nenia el tiuj &ccirc;i provoj <klr>[&#x2026;]</klr> eliris el la <tld/>o de projektoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;io, kio aperas en la <tld/>o de la homa penso, estus tradukata
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        grandaj ideoj en la <tld/>o de moralo, filozofio kaj scienco
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu odora <tld/>o en la regno de la belo havas sian publikon
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malri&ccirc;aj virinoj estas el&jcirc;etitaj el la <tld/>o de industrio
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la <tld/>o de lia konateco ekzistis plu neniu, pri kiu li povus svati&gcirc;i
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolumo</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la <tld/>o de la arto, same kiel en &ccirc;iu alia <tld/>o, kiu prezentas kampon
        por homa laborado
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        Marta venis el la <tld/>o de feli&ccirc;a virina senscieco
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        senkorpaj fantomoj en la <tld/>o de revoj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sepa Kongreso Esperantista en Antwerpen en la 21a de a&ubreve;gusto 1911</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_o;&c_b;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol; 
      <klr>
      (&c_d;&c_z;&c_je;&c_j;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;&c_ib;)
      </klr></trd>,
      <trd>&c_s;&c_f;&c_e;&c_r;&c_a; <klr>
      (&c_d;&c_z;&c_je;&c_j;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;&c_ib;)
      </klr></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Sph&auml;re</trd>,
      <trd>Gebiet</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>ter&uuml;let</trd>,
      <trd>r&eacute;gi&oacute;</trd>
    </trdgrp>
    <trd lng="nl">gebied</trd>
    <trdgrp lng="pl">
      <trd>region</trd>,
      <trd>rejon</trd>,
      <trd>chor&aogonek;giew<klr> (w harcerstwie)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_l;&c_a;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_f;&c_je;&c_r;&c_a;</trd>
    </trdgrp>
  </snc>
