<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>komoci</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="komoci.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      &Scirc;okegi ies korpon, ka&ubreve;zante momentan senmovecon a&ubreve; perturbante
      organon:
      <ekz>
        la vasko ku&scirc;is tiel <tld/>ita, ke li ne povis respondi e&ccirc; unu
        vorton<fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 9a, p.
        75a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kontuz.0i">kontuzi</ref>
    </refgrp>
  </snc>
  <trd lng="pt">concutir</trd>
  <trd lng="fr">commotionner</trd>
  <trd lng="ru">&c_v;&c_y;&c_z;&c_v;&c_a;&c_t;&c_mol; <ind>&c_s;&c_o;&c_t;&c_r;&c_ja;&c_s;&c_je;&c_n;&c_i;&c_je;</ind><klr> (&c_m;&c_je;&c_d;.)</klr></trd>
</drv>

<drv mrk="komoci.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Sekvoj de korpa &scirc;okego, kiel doloro, senmoveco ktp.
    </dif>
  </snc>
  <trdgrp lng="pt">
    <trd>concuss&atilde;o</trd>,
    <trd>como&ccedil;&atilde;o</trd>
  </trdgrp>
  <trd lng="fr">commotion</trd>
  <trd lng="ru">&c_s;&c_o;&c_t;&c_r;&c_ja;&c_s;&c_je;&c_n;&c_i;&c_je;<klr> (&c_m;&c_je;&c_d;.)</klr></trd>  
</drv>
</art>

