<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
<kap>
 <ofc>*</ofc>
 <rad>salut</rad>/i
</kap>
<drv mrk="salut.0i">
<kap><ofc>*</ofc><tld/>i</kap>
<snc>
   <dif>
     Montri al iu per ekstera &gcirc;entila signo sian respekton,
     estimon, &scirc;aton: <ekz>klini <tld/>e la kapon<fnt>Z</fnt>.  </ekz>
   </dif>
   <trdgrp lng="be">
     <trd>&c_v;&c_ib;&c_t;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_p;&c_r;&c_y;&c_v;&c_ib;&c_t;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_p;&c_a;&c_v;&c_ib;&c_t;&c_a;&c_c;&c_mol;</trd>
   </trdgrp>
   <trd lng="cs">(po)zdravit</trd>
   <trd lng="de">gr&uuml;&szlig;en</trd>
   <trd lng="en">greet</trd>
   <trd lng="nl">groeten</trd>
   <trd lng="ru">&c_p;&c_r;&c_i;&c_v;&c_je;&c_t;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</snc>
<snc>
   <uzo tip="stl">FIG</uzo>
   <dif> Deklari publike sian admiron, respekton,
      intereson, kontentecon pri io: <ekz>en via nomo mi esprimas
      funebran <tld/>on al la ombroj de niaj kamaradoj<fnt>Z</fnt>;
      </ekz> <ekz>ni &gcirc;oje <tld/>is tiun decidon, la aperon de tiu
      libro.</ekz>
   </dif>
   <trd lng="be">&c_v;&c_ib;&c_t;&c_a;&c_c;&c_mol;</trd>
   <trd lng="cs">(po)zdravit</trd>
   <trd lng="en">salute</trd>
   <trd lng="nl">groeten</trd>
   <trd lng="ru">&c_p;&c_r;&c_i;&c_v;&c_je;&c_t;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</snc>
<snc mrk="salut.0i.komp">
   <uzo tip="fak">KOMP</uzo>
   <dif>Komenci seancon kun dialoga programo, eventuale sciigante al la
   komputilo sian nomon kaj eble anka&ubreve; la pasvorton.
   </dif>
   <ref tip="ant" cel="adiaux.0i.komp">adia&ubreve;i</ref>
   <ref tip="malprt" cel="seanc.0o.komp">seanco</ref>
   <trd lng="be">&c_u;&c_v;&c_a;&c_h;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol; <klr>(&c_u; &c_k;&c_a;&c_m;&c_p;&c_u;&c_t;&c_a;&c_r;&c_n;&c_u;&c_ju; &c_s;&c_ib;&c_s;&c_t;&c_e;&c_m;&c_u;, &c_s;&c_je;&c_t;&c_k;&c_u;)</klr></trd>
   <trd lng="de">sich <ind>einloggen</ind></trd>
   <trd lng="en">login</trd>
   <trd lng="fr">se <ind>connecter</ind></trd>
   <trd lng="nl">inloggen</trd>
   <trd lng="ru">&c_v;&c_o;&c_j;&c_t;&c_i;</trd>
</snc>
</drv>
<drv mrk="salut.0nomo">
<kap><ofc>*</ofc><tld/>nomo</kap>
<snc>
   <uzo tip="fak">KOMP</uzo>
   <dif>En komputila sistemo por pluraj uzantoj, la vorto kiun uzanto
   indikas dum saluto.
   </dif>
   <ref tip="malprt" cel="kont.0o">konto</ref>
   <trd lng="be">&c_ib;&c_m;&c_ja; <klr>(&c_k;&c_a;&c_r;&c_y;&c_s;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;&c_a; &c_k;&c_a;&c_m;&c_p;&c_u;&c_t;&c_a;&c_r;&c_n;&c_a;&c_j; &c_s;&c_ib;&c_s;&c_t;&c_e;&c_m;&c_y;, &c_s;&c_je;&c_t;&c_k;&c_ib;)</klr></trd>
   <trd lng="de">Benutzername</trd>
   <trd lng="en">login name</trd>
   <trd lng="fr">identificateur</trd>
   <trd lng="nl">gebruikersnaam</trd>
<trd lng="ru">&c_i;&c_m;&c_ja; &c_v;&c_h;&c_o;&c_d;&c_a; &c_v; &c_s;&c_i;&c_s;&c_t;&c_je;&c_m;&c_u;</trd>
</snc>
</drv>
</art>
