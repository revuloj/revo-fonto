<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>salut</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="salut.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc mrk="salut.0i.MOR">
    <dif>
      Montri al iu per ekstera &gcirc;entila signo sian respekton,
      estimon, &scirc;aton:
      <ekz>
        klini <tld/>e la kapon<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_ib;&c_t;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_y;&c_v;&c_ib;&c_t;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_v;&c_ib;&c_t;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="cs">(po)zdravit</trd>
    <trd lng="de">gr&uuml;&szlig;en</trd>
    <trd lng="en">greet</trd>
    <trd lng="es">saludar</trd>
    <trd lng="fr">saluer</trd>
    <trdgrp lng="hu">
      <trd>k&ouml;sz&ouml;n</trd>,
      <trd>&uuml;dv&ouml;z&ouml;l</trd>
    </trdgrp>
    <trd lng="it">salutare</trd>
    <trd lng="nl">groeten</trd>
    <trdgrp lng="pl">
      <trd>wita&cacute;</trd>,
      <trd>pozdrawia&cacute;</trd>
    </trdgrp>
  <trdgrp lng="pt">
   <trd>saudar</trd>
  </trdgrp>
    <trd
    lng="ru">&c_p;&c_r;&c_i;&c_v;&c_je;&c_t;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    <trd lng="sv">h&auml;lsa<klr tip="ind"> (verb)</klr></trd>
  </snc>
  <snc mrk="salut.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Deklari publike sian admiron, respekton,
      intereson, kontentecon pri io:
      <ekz>
        en via nomo mi esprimas
        funebran <tld/>on al la ombroj de niaj kamaradoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ni &gcirc;oje <tld/>is tiun decidon, la aperon de tiu libro.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_ib;&c_t;&c_a;&c_c;&c_mol;</trd>
    <trd lng="cs">(po)zdravit</trd>
    <trd lng="en">salute</trd>
    <trd lng="fr">saluer</trd>
    <trd lng="hu">k&ouml;sz&ouml;nt</trd>
    <trd lng="it">salutare</trd>    
    <trd lng="nl">groeten</trd>
    <trdgrp lng="pl">
      <trd>wita&cacute;</trd>,
      <trd>pozdrawia&cacute;</trd>
    </trdgrp>
    <trd
    lng="ru">&c_p;&c_r;&c_i;&c_v;&c_je;&c_t;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="salut.0i.komp">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Komenci seancon kun dialoga programo, eventuale sciigante al la
      komputilo sian nomon kaj eble anka&ubreve; la pasvorton;
      <ref tip="sin" cel="salut.en0i">en<tld/>i</ref>
    </dif>
    <ref tip="ant" cel="adiaux.0i.komp">adia&ubreve;i</ref>
    <ref tip="malprt" cel="seanc.0o.komp">seanco</ref>
    <trd lng="be">&c_u;&c_v;&c_a;&c_h;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol;
    <klr>(&c_u; &c_k;&c_a;&c_m;&c_p;&c_u;&c_t;&c_a;&c_r;&c_n;&c_u;&c_ju;
    &c_s;&c_ib;&c_s;&c_t;&c_e;&c_m;&c_u;,
    &c_s;&c_je;&c_t;&c_k;&c_u;)</klr></trd>
    <trd lng="de">sich <ind>einloggen</ind></trd>
    <trd lng="en">login</trd>
    <trdgrp lng="es">
      <trd>identificarse <klr>(poner usuario y contrase&ntilde;a)</klr></trd>,
      <trd>iniciar sesi&oacute;n</trd>
    </trdgrp>
    <trd lng="fr">se <ind>connecter</ind></trd>
    <trd lng="hu">bejelentkezik</trd>
    <trd lng="it">login</trd>
    <trd lng="nl">inloggen</trd>
    <trdgrp lng="pl">
      <trd>logowa&cacute; si&eogonek;<klr> do systemu</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_o;&c_j;&c_t;&c_i;</trd>
    <trd lng="sv">logga in</trd>
  </snc>
  <trdgrp lng="sk">
    <trd>odzdravi큐; <klr>(od)</klr>salutova큐</trd>,
    <trd>pozdravi큐</trd>
  </trdgrp>
</drv>

<drv mrk="salut.0o">
  <kap><tld/>o</kap>
  <snc mrk="salut.0o.MOR">
    <dif>
      Ago <tld/>i;
      <klr>(la vorto estas ofte uzata akuzative
      kiel respekta alparolo okaze de renkonto)</klr>:
      <ekz>
        <ind><tld/>on!</ind>
        <trdgrp lng="en">
          <trd>hello!</trd>,
          <trd>hi!</trd>,
          <trd>how do you do?</trd>
        </trdgrp>
        <trdgrp lng="de">
          <trd>guten Morgen!</trd>,
          <trd>guten Tag!</trd>,
          <trd>guten Abend!</trd>
        </trdgrp>
        <trd lng="es">&#161;hola!</trd>
        <trdgrp lng="fr">
          <trd>bonjour!</trd>,
          <trd>salut!</trd>
        </trdgrp>
        <trd lng="hu">szervusz</trd>
        <trdgrp lng="it">
           <trd>ciao</trd>,
           <trd>salve</trd>
        </trdgrp>
        <trdgrp lng="pl">
          <trd>hej!</trd>,
          <trd>cze&sacute;&cacute;!</trd>,
          <trd>witaj!<klr> {sing.}</klr></trd>,
          <trd>witajcie!<klr> {pl.}</klr></trd>,
          <trd>czo&lstroke;em<klr> (pot.)</klr></trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_z;&c_d;&c_r;&c_a;&c_v;&c_s;&c_t;&c_v;&c_u;&c_j;&c_t;&c_je;!</trd>,
          <trd>&c_z;&c_d;&c_r;&c_a;&c_v;&c_s;&c_t;&c_v;&c_u;&c_j;!</trd>,
          <trd>&c_p;&c_r;&c_i;&c_v;&c_je;&c_t;!</trd>
        </trdgrp>
        <trdgrp lng="sv">
          <trd>hej!</trd>,
          <trd>hejsan!</trd>,
          <trd>hall&aring;!</trd>
        </trdgrp>
        <trd lng="zh">&#20320;&#22909;<klr> [n&#464;
        h&#462;o]</klr></trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Gru&szlig;</trd>,
    <trd>Begr&uuml;&szlig;ung</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>greeting</trd>,
    <trd>salute</trd>,
    <trd>salutation</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>saludo</trd>,
    <trd>salutaci&oacute;n</trd>,
    <trd>hola</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>salutation</trd>,
    <trd>salut</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&ouml;sz&ouml;nt&eacute;s</trd>,
    <trd>&uuml;dv&ouml;zl&eacute;s</trd>,
    <trd>&uuml;dv&ouml;zlet</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd><klr>(il) </klr>salutare</trd>,
    <trd>saluto</trd>
  </trdgrp>
  <trd
  lng="ru">&c_p;&c_r;&c_i;&c_v;&c_je;&c_t;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  <trd lng="sk">znak za훾iatku</trd>
  <trd lng="sv">h&auml;lsning</trd>
</drv>

<drv mrk="salut.0inda">
  <kap><tld/>inda</kap>
  <snc mrk="salut.0inda.KOMUNE">
    <dif>
      Kiu indas <tld/>on, kiu estas bonvena,
      meritas publikan agnoskon:
      <ekz>
        la provo starigi universalan skribon estas <tld/>inda
        <fnt>
          <aut>R. Corsetti</aut>,
          <vrk><url ref="http://www.nooalf.com/eSPURoNTO.htm">
          Facila alfabeto...</url></vrk>,
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gratul.0inda.KOMUNE">gratulinda</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>bienvenu</trd>,
    <trd>opportun</trd>
  </trdgrp>
</drv>

<drv mrk="salut.el0i">
  <kap>el<tld/>i</kap>
  <snc mrk="salut.el0i.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <ref tip="dif" cel="adiaux.0i.komp">adia&ubreve;i<sncref/></ref>
    <ekz>
      vi nun estas el<tld/>inta
      <fnt>
        <aut>-</aut>,
        <vrk><url ref="http://eo.wikibooks.org">
        Vikilibroj</url></vrk>,
        <lok>[vidita en 2013]</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="de">
    <trd>abmelden</trd>,
    <trd>ausloggen</trd>
  </trdgrp>
  <trd lng="en">logout</trd>
  <trd lng="es">cerrar sesi&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>fermer la session<klr tip="ind"> (info.)</klr></trd>,
    <trd>se <ind>d&eacute;loguer</ind></trd>
  </trdgrp>
  <trd lng="he">&lamed;&he;&tav;<ind>&nun;&tav;&qof;</ind><klr tip="amb">
(&bet;&mem;&het;&shin;&bet;&yod;&fmem;)</klr></trd>
  <trd lng="hu">kijelentkezik</trd>
  <trd lng="it">logout</trd>    
  <trd lng="nl">uitloggen</trd>
  <trd lng="pl">wylogowa&cacute;</trd>
  <trd lng="pt"><ind>desligar</ind> o microcomputador</trd>
  <trd lng="ru">&c_v;&c_y;&c_j;&c_t;&c_i;</trd>
  <trd lng="sk">odhl&aacute;si&tcaron; sa</trd>
  <trd lng="sv"><ind>logga</ind> ut</trd>
</drv>

<drv mrk="salut.en0i">
  <kap>en<tld/>i</kap>
  <snc mrk="salut.en0i.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <ref tip="dif" cel="salut.0i.komp"><tld/>i<sncref/></ref>
    <ekz>
      gasto estas ajna persono <klr>...</klr>,
      kiu ne en<tld/>is en la forumon
        <fnt>
          <aut>-</aut>,
          <vrk><url
          ref="http://wiki.simplemachines.org/smf/Registering/eo">
          Simple Machines</url></vrk>,
          <lok>2011</lok>
        </fnt>
    </ekz>
  </snc>
  <trd lng="be">&c_u;&c_v;&c_a;&c_h;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol;
  <klr>(&c_u; &c_k;&c_a;&c_m;&c_p;&c_u;&c_t;&c_a;&c_r;&c_n;&c_u;&c_ju; &c_s;&c_ib;&c_s;&c_t;&c_e;&c_m;&c_u;, &c_s;&c_je;&c_t;&c_k;&c_u;)</klr></trd>
  <trd lng="de">sich <ind>einloggen</ind></trd>
  <trd lng="en">login</trd>
  <trdgrp lng="es">
    <trd>identificarse <klr>(poner usuario y contrase&ntilde;a)</klr></trd>,
    <trd>iniciar sesi&oacute;n</trd>
  </trdgrp>
  <trd lng="fr">se <ind>connecter</ind></trd>
  <trd lng="hu">bejelentkezik</trd>
  <trd lng="it">login</trd>
  <trd lng="nl">inloggen</trd>
  <trdgrp lng="pl">
    <trd>logowa&cacute; si&eogonek;<klr> do systemu</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_j;&c_t;&c_i;</trd>
  <trd lng="sv">logga in</trd>
</drv>

<drv mrk="salut.kap0o">
  <kap>kap<tld/>o</kap>
  <snc mrk="salut.kap0o.MOR">
    <dif>
      <ref tip="super" cel="salut.0o.MOR"><tld lit="S"/>o</ref>
      per movo, ordinare klineto de la kapo:
      <ekz>
        &scirc;i faris facilan kap<tld/>on al la mastrino kaj al Marta
        kaj foriris
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">signe de t&ecirc;te<klr> (salut)</klr></trd>
  <trd lng="hu"><klr>(&uuml;dv&ouml;zl&odblac;)</klr>
  fejbiccent&eacute;s</trd>
  <trd lng="it"><ind>cenno</ind> del capo<klr> (saluto)</klr></trd>
  <trdgrp lng="pl">
    <trd>skini&eogonek;cie<klr> g&lstroke;ow&aogonek;</klr></trd>,
    <trd>uk&lstroke;on</trd>
  </trdgrp>
  <trd lng="sv">nicka<klr> (h&auml;lsning)</klr></trd>
</drv>

<drv mrk="salut.klin0o">
  <kap>klin<tld/>o</kap>
  <snc mrk="salut.klin0o.MOR">
    <dif>
      <ref tip="super" cel="salut.0o.MOR"><tld lit="S"/>o</ref> per
      klino de la busto:
      <ekz>
        li bonvenigis Lemuelon kun multaj profundaj klin<tld/>oj
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 2a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">salut<klr> (en s'inclinant)</klr></trd>
  <trdgrp lng="hu">
    <trd>f&odblac;hajt&aacute;s</trd>,
    <trd>meghajl&aacute;s</trd>
  </trdgrp>
  <trd lng="it">inchino</trd> 
  <trdgrp lng="pl">
    <trd>uk&lstroke;on</trd>,
    <trd>dygni&eogonek;cie</trd>
  </trdgrp>
  <trd lng="sv">buga sig<klr> (som en h&auml;lsning)</klr></trd>
</drv>

<drv mrk="salut.0nomo">
  <kap><ofc>*</ofc><tld/>nomo</kap>
  <snc mrk="salut.0nomo.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      En komputila sistemo por pluraj uzantoj, la vorto kiun uzanto
      indikas dum saluto.
    </dif>
    <ref tip="malprt" cel="kont.0o">konto</ref>
    <trd lng="be">&c_ib;&c_m;&c_ja;
    <klr>(&c_k;&c_a;&c_r;&c_y;&c_s;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;&c_a;
    &c_k;&c_a;&c_m;&c_p;&c_u;&c_t;&c_a;&c_r;&c_n;&c_a;&c_j;
    &c_s;&c_ib;&c_s;&c_t;&c_e;&c_m;&c_y;,
    &c_s;&c_je;&c_t;&c_k;&c_ib;)</klr></trd>
    <trd lng="de">Benutzername</trd>
    <trd lng="en">login name</trd>
    <trd lng="es">nombre de usuario</trd>
    <trdgrp lng="fr">
      <trd>login</trd>,
      <trd>identifiant d'utilisateur<klr> (login)</klr></trd>,
      <trd>nom d'utilisateur<klr> (login)</klr></trd>
    </trdgrp>
    <trd lng="hu">bejelentkez&eacute;si n&eacute;v</trd>
    <trd lng="it">login<klr> (nome utente)</klr></trd>    
    <trd lng="nl">gebruikersnaam</trd>
    <trdgrp lng="pl">
      <trd>nazwa u&zdot;ytkownika</trd>,
      <trd>login</trd>,
      <trd>nazwa logowania</trd>
    </trdgrp>
    <trd lng="ru">&c_i;&c_m;&c_ja; &c_v;&c_h;&c_o;&c_d;&c_a; &c_v;
    &c_s;&c_i;&c_s;&c_t;&c_je;&c_m;&c_u;</trd>
    <trdgrp lng="sv">
      <trd>inloggningsnamn</trd>,
      <trd>anv&auml;ndarnamn</trd>
    </trdgrp>
  </snc>
  <trd lng="sk">meno pou탑챠vate컁a</trd>
</drv>
</art>
