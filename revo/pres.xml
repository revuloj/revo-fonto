  <trdgrp lng="ja">
    <trd>&#x5370;&#x5237;&#x5668;  <pr>&#x3044;&#x3093;&#x3055;&#x3064;&#x3046;&#x3064;&#x308F;</pr></trd>,
    <trd>&#x30D7;&#x30EA;&#x30F3;&#x30BF;</trd>
  </trdgrp>
