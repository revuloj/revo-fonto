<drv mrk="pres.re0o">
  <kap>re<tld/>o</kap>
  <snc mrk="pres.re0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      <tld lit="P"/>a republikigo de verko, eventuale en nova formato,
      kun ne&scirc;an&gcirc;ita enhavo:
      <ekz>
        epokfara traduko <klr>[&#x2026;]</klr> denove havebla pro
        &jcirc;usa
        re<tld/>o
        <fnt><bib>Plumamikoj</bib>, <lok>tradukoj</lok></fnt>;
      </ekz>
      <ekz>
        la intencoj de dramo, kies celo &ccirc;iam estis, estas kaj
        estos:
        prezenti kvaza&ubreve; spegulon de la naturo, montri al la
        <klr>[&#x2026;]</klr>
        responda tempo la re<tld/>on de &gcirc;ia formo
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        La Nova Testamento <klr>[&#x2026;]</klr>, eldonita de La Brita
        kaj alilanda Biblia Societo, jam en 1912, kun tuja re<tld/>o en
        1913
        <fnt>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2005/008553.php">Ankora&ubreve;
          Sanktaj Skriboj</url></vrk>
           <bib>Monato</bib>
          <lok>2005/05, p. 21</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia verko preska&ubreve; forgesita restas nun sen re<tld/>oj
        <fnt>
          <aut>Andreo Lovaniensis</aut>
          <vrk><url ref="&Monato;/2011/010174.php">Surpriza
          proverbaro</url></vrk>
          <bib>Monato</bib>
          <lok>2011/01, p. 21</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Neuauflage</trd>,
      <trd>Reprint</trd>,
      <trd>Neufassung</trd>,
      <trd>Reproduktion</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_je;&c_r;&c_a;&c_d;&c_r;&c_u;&c_k;</trd>,
    <trd>&c_r;&c_e;&c_p;&c_r;&c_y;&c_n;&c_t;</trd>
  </trdgrp>
