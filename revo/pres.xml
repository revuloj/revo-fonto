<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>pres</rad>/i
    </kap>

    <drv mrk="pres.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="pres.0i.TIP">
        <uzo tip="fak">TIP</uzo>
        <dif>
          Reprodukti tekston, bildojn ktp, kutime grandkvante kaj
          &gcirc;enerale, per apliko de inko al papero (historie, per
          gravuritaj tabuloj a&ubreve; metalaj literoj &scirc;miritaj per inko:
          <ekz>
            <tld/>i artikolon en revuo, flora&jcirc;ojn sur &scirc;tofo;
          </ekz><ekz>
            <tld/>ejo.
          </ekz>
        </dif>
      </snc>
      <snc mrk="pres.0i.KOMP">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          <ref tip="dif" cel="print.0i.KOMP">Printi</ref>.
        </dif>
        <rim>
          Multaj lingvoj uzas unu saman vorton por
          <tld/>i_<sncref ref="pres.0i.TIP"/> kaj printi; tamen
          nemalmultaj esperantistoj sentas bezonon apartigi
          publikigan sencon netan (<tld/>i_<sncref ref="pres.0i.TIP"/>)
          kaj la privatan, ofte malnetan kaj unuekzempleran
          printadon.  En pli frua epoko pro simila precizemo en
          Esperanto aparti&gcirc;is la vorto <ref cel="tajp.0i">tajpi</ref>,
          kvankam anka&ubreve; &gcirc;in multaj lingvoj ne distingigas disde
          <tld/>i.
          <aut>Sergio Pokrovskij</aut>
        </rim>
      </snc>
      <ref tip="super" cel="skrib.0i.signojn">skribi</ref>
      <trd lng="cs">tisknout</trd>
      <trd lng="de">drucken</trd>
      <trd lng="en">print</trd>
      <trd lng="nl">drukken</trd>
      <trd lng="ru">&c_p;&c_je;&c_ch;&c_a;&c_t;&c_a;&c_t;&c_mol;</trd>
    </drv>

    <drv mrk="pres.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Ago <tld/>i:
          <ekz>
            la re<tld/>o de tiu libro estas nun komencita;
          </ekz><ekz>
            <ind><tld/>eraro</ind>;
            <trd lng="nl">drukfout</trd>
          </ekz><ekz>
            <ind><tld/>prova&jcirc;o</ind>
            <klr>(unua <tld/>a&jcirc;o, sur kiu oni korektas la erarojn).</klr>
            <trd lng="nl">drukproef</trd>
          </ekz> 
        </dif>
        <trd lng="nl">druk</trd>
      </snc>
      <snc>
        <dif>
          Arto <tld/>i; profesio de la <tld/>istoj. 
        </dif>
        <trd lng="nl">drukkunst</trd>
      </snc>
      <trd lng="de">Druck</trd>
    </drv>

    <drv mrk="pres.0a">
      <kap><tld/>a</kap>
      <snc>
        <dif>
          Rilata al <tld/>arto, uzata en <tld/>arto:
          <ekz>
            <tld/>a litero<fnt>Z</fnt>.
          </ekz>  
        </dif>
      </snc>
      <trd lng="nl">druk-</trd>
    </drv>

    <drv mrk="pres.0ajxo">
      <kap><tld/>a&jcirc;o</kap>
      <snc>
        <dif>
          Io, kio estas <tld/>ita.
        </dif>
      </snc>
      <trd lng="nl">drukwerk</trd>
    </drv>

    <drv mrk="pres.0ejo">
      <kap><tld/>ejo</kap>
      <snc>
        <dif>
          Konstrua&jcirc;o kie oni <tld/>as:
          <ekz>
            Flandra Esperanto-Ligo disponas
            pri propra kompostejo kaj <tld/>ejo.
          </ekz>  
        </dif>
      </snc>
      <trd lng="nl">drukkerij</trd>
    </drv>
  </art>
