<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mi.xml,v 1.35 2017/12/19 21:10:18 revo Exp $">
<kap>
   <ofc>*</ofc> <rad>mi</rad> <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="mi.0">
   <kap><ofc>*</ofc><tld/></kap>
   <snc>
     <dif>
       <ref tip="super" cel="pronom.0o">Pronomo</ref>
       de la unua persono en ununombro:
       <ekz>
         <tld/>a, <tld/>n kaj <tld/> en mondo re&gcirc;as tri<fnt>Z</fnt>.
       </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_ja;</trd>
  <trd lng="bg">&c_a;&c_z;</trd>
  <trd lng="br">me</trd>
  <trdgrp lng="ca">
    <trd>jo</trd>,
    <trd>mi</trd>
  </trdgrp>
  <trd lng="cs">m&iacute; <klr>(&rcaron;eck&eacute; p&iacute;smeno)</klr></trd>
  <trd lng="de">ich</trd>
  <trdgrp lng="en">
    <trd>I</trd>,
    <trd>me</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>yo</trd>,
    <trd>mi</trd>
  </trdgrp>
  <trd lng="fi">min&auml;</trd>
  <trd lng="fr">je</trd>
  <trd lng="fy">ik</trd>
  <trd lng="he">&alef;&nun;&yod;</trd>
  <trd lng="hr">ja</trd>
  <trd lng="hu">&eacute;n</trd>
  <trd lng="ia">io</trd>
  <trdgrp lng="id">
     <trd>aku</trd>,
     <trd>saya</trd>
  </trdgrp>
  <trd lng="io">me</trd>
  <trd lng="it">io</trd>
  <trdgrp lng="ja">
    <trd>&#31169; [<ind>&#12431;&#12383;&#12375;</ind>]</trd>
    <trd>&#20693; [<ind>&#12412;&#12367;</ind>]</trd>
    <trd>&#12354;&#12383;&#12375;</trd>
  </trdgrp>
  <trd lng="ku">ez</trd>
  <trd lng="nl">ik</trd>
  <trd lng="pl">ja</trd>
  <trd lng="pt">eu</trd>
  <trd lng="ru">&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>ja</trd>,
    <trd>m&iacute; <klr>(gr&eacute;cke p&iacute;smeno)</klr></trd>
  </trdgrp>
  <trd lng="sv">jag</trd>
  <trd lng="tp">mi</trd>
  <trd lng="vo">ob</trd>
  <trdgrp lng="zh">
    <trd>&#25105; [<ind>w&#466;</ind>]</trd>
    <trd>&#21566; [<ind>w&uacute;</ind>]</trd>
  </trdgrp>
</drv>

<drv mrk="mi.0a">
  <kap><ofc>*</ofc><tld/>a <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url
  ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5112">mi'a</url></lok></fnt></kap>
  <snc>
    <dif>
      Poseda adj. a&ubreve; pron. signifanta: &leftquot;posedata de mi, rilata 
al mi&rightquot;.
    </dif>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_m;&c_o;&c_j;</trd>,
    <trd>&c_m;&c_i;</trd>
  </trdgrp>
  <trd lng="ca">meu</trd>
  <trdgrp lng="cs">
    <trd>moje</trd>,
    <trd>m&uring;j</trd>
  </trdgrp>
  <trd lng="de">mein</trd>
  <trd lng="en">my</trd>
  <trdgrp lng="fi">
    <trd>minun ... -ni</trd>,
    <trd>-<ind>ni</ind></trd>
  </trdgrp>
  <trd lng="fy">myn</trd>
  <trdgrp lng="id">
    <trd><ind>milik</ind>ku</trd>,
    <trd><ind>milik</ind> saya</trd>
  </trdgrp>
  <trd lng="io">mea</trd>
  <trdgrp lng="ja">
    <trd>&#31169;&#12398; [<ind>&#12431;&#12383;&#12375;&#12398;</ind>]</trd>
    <trd>&#20693;&#12398; [<ind>&#12412;&#12367;&#12398;</ind>]</trd>
    <trd>&#12354;&#12383;&#12375;&#12398;</trd>
  </trdgrp>
  <trd lng="la">meus</trd>
  <trd lng="nl">mijn</trd>
  <trd lng="pl">m&oacute;j</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_j;</trd>,
    <trd>&c_m;&c_o;&c_ja;</trd>
    <trd>&c_m;&c_o;&c_jo;</trd>
    <trd>&c_m;&c_o;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>moje</trd>,
    <trd>m&ocirc;j</trd>
  </trdgrp>
  <trd lng="tp">mi</trd>
  <trdgrp lng="vo">
    <trd>obik</trd>,
    <trd>oba</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#25105; [<ind>w&#466;</ind>] <klr tip="ind">(adj.)</klr></trd>
    <trd>&#25105;&#30340; [<ind>w&#466; de</ind>]</trd>
  </trdgrp>
</drv>
</art>
