    <trd>self<klr> (noun)</klr></trd>
  </trdgrp>
  <trd lng="fr">le <ind>moi</ind><klr> (subst.)</klr></trd>
  <trd lng="uk">&c_s;&c_v;&c_o;&c_jeu;/&c_v;&c_l;&c_a;&c_s;&c_n;&c_je; &rightquot;&c_ja;&#x201D;</trd>
