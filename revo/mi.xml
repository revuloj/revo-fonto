<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc> <rad>mi</rad> <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="mi.0">
   <kap><ofc>*</ofc><tld/></kap>
   <snc>
     <dif>
       <ref tip="super" cel="pronom.0o">Pronomo</ref>
       de la unua persono en ununombro:
       <ekz>
         <tld/>a, <tld/>n kaj <tld/> en mondo re&gcirc;as tri<fnt>Z</fnt>.
       </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_ja;</trd>
  <trd lng="bg">&c_a;&c_z;</trd>
  <trd lng="br">me</trd>
  <trdgrp lng="ca">
     <trd>jo</trd>,
     <trd>mi</trd>
   </trdgrp>
  <trd lng="cs">mí <klr>(řecké písmeno)</klr></trd>
  <trd lng="de">ich</trd>
  <trdgrp lng="en">
     <trd>I</trd>,
     <trd>me</trd>
   </trdgrp>
  <trdgrp lng="es">
     <trd>yo</trd>,
     <trd>mi</trd>
   </trdgrp>
  <trd lng="fi">min&auml;</trd>
  <trd lng="fr">je</trd>
  <trd lng="he">&alef;&nun;&yod;</trd>
  <trd lng="hr">ja</trd>
  <trd lng="hu">&eacute;n</trd>
  <trd lng="ia">io</trd>
  <trdgrp lng="id">
     <trd>aku</trd>,
     <trd>saya</trd>
   </trdgrp>
  <trd lng="it">io</trd>
  <trd lng="ku">ez</trd>
  <trd lng="nl">ik</trd>
  <trd lng="pl">ja</trd>
  <trd lng="pt">eu</trd>
  <trd lng="ru">&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>ja</trd>,
    <trd>mí <klr>(grécke písmeno)</klr></trd>
  </trdgrp>
  <trd lng="sv">jag</trd>
  <trd lng="tp">mi</trd>
  <trd lng="vo">ob</trd>
  <trd lng="zh">&#25105;<klr> [w&#466;]</klr></trd>
</drv>

<drv mrk="mi.0a">
   <kap><tld/>a</kap>
   <snc>
     <dif>
       Poseda adj. a&ubreve; pron. signifanta: &leftquot;posedata de mi, rilata 
al mi&rightquot;.
     </dif>
   </snc>
  <trdgrp lng="bg">
     <trd>&c_m;&c_o;&c_j;</trd>,
     <trd>&c_m;&c_i;</trd>
   </trdgrp>
  <trd lng="ca">meu</trd>
  <trdgrp lng="cs">
    <trd>moje</trd>,
    <trd>můj</trd>
  </trdgrp>
  <trd lng="de">mein</trd>
  <trd lng="en">my</trd>
  <trdgrp lng="fi">
     <trd>minun ... -ni</trd>,
     <trd>-<ind>ni</ind></trd>
   </trdgrp>
  <trdgrp lng="id">
     <trd><ind>milik</ind>ku</trd>,
     <trd><ind>milik</ind> saya</trd>
   </trdgrp>
  <trd lng="la">meus</trd>
  <trd lng="pl">m&oacute;j</trd>
  <trdgrp lng="ru">
     <trd>&c_m;&c_o;&c_j;</trd>,
     <trd>&c_m;&c_o;&c_ja;</trd>
     <trd>&c_m;&c_o;&c_jo;</trd>
     <trd>&c_m;&c_o;&c_i;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>moje</trd>,
    <trd>môj</trd>
  </trdgrp>
  <trd lng="tp">mi</trd>
</drv>
</art>
