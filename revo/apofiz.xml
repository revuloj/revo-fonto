<vortaro>
<art mrk="$Id$">
<kap><rad>apofiz</rad>/o</kap>
<drv mrk="apofiz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Elstara&jcirc;o de <ref tip="malprt" cel="ost.0o">osto</ref>:
      <ekz>
        akcesora, dorneca, frunta, tempia <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="diafiz.0o">diafizo</ref>,
      <ref cel="epifiz.0o">epifizo</ref>.
    </refgrp>
  </snc>
  <trd lng="ca">ap&ograve;fisi</trd>
  <trdgrp lng="de">
    <trd>Apophyse</trd>,
    <trd>Apophysis</trd>,
    <trd>sekund&auml;re Epiphyse</trd>,
    <trd>sekund&auml;re Epiphysis</trd>,
    <trd>Processus</trd>,
    <trd>Knochenfortsatz</trd>,
    <trd>vorspringender Knochenfortsatz</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>apophysis</trd>,
    <trd>process</trd>
  </trdgrp>
  <trd lng="fr">apophyse</trd>
  <trdgrp lng="hu">
    <trd>apofizis</trd>,
    <trd>kin&ouml;v&eacute;s<klr tip="ind"> (csontv&eacute;g)</klr></trd>
  </trdgrp>
  <trd lng="nl">apofyse</trd>
  <trd lng="pl">apofiza</trd>
  <trd lng="ru">&c_a;&c_p;&c_o;&c_f;&c_i;&c_z;</trd>
  <trdgrp lng="sk">
    <trd>apof&yacute;za</trd>,
    <trd>v&yacute;rastok<klr> (kosti)</klr></trd>
  </trdgrp>
</drv>
</art>
