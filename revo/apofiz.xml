<kap><rad>apofiz</rad>/o</kap>
<drv mrk="apofiz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Elstara&jcirc;o de <ref tip="malprt" cel="ost.0o">osto</ref>:
      <ekz>
        akcesora, dorneca, frunta, tempia <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="diafiz.0o">diafizo</ref>,
      <ref cel="epifiz.0o">epifizo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_o;&c_f;&c_ib;&c_z;</trd>
  <trd lng="ca">ap&ograve;fisi</trd>
  <trdgrp lng="cs">
    <trd>apof&#xFD;za</trd>,
    <trd>od&#x17E;ilek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Apophyse</trd>,
    <trd>Apophysis</trd>,
    <trd>sekund&auml;re Epiphyse</trd>,
    <trd>sekund&auml;re Epiphysis</trd>,
    <trd>Processus</trd>,
    <trd>Knochenfortsatz</trd>,
    <trd>vorspringender Knochenfortsatz</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>apophysis</trd>,
    <trd>process</trd>
  </trdgrp>
  <trd lng="es">ap&oacute;fisis</trd>
  <trd lng="fr">apophyse</trd>
  <trdgrp lng="he">
    <trd>&#x5D1;&#x5DC;&#x5D9;&#x5D8;&#x5D4;</trd>,
    <trd>&#x5D6;&#x5D9;&#x5D6;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>apofizis</trd>,
    <trd>kin&ouml;v&eacute;s<klr tip="ind"> (csontv&eacute;g)</klr></trd>
  </trdgrp>
  <trd lng="it">apofisi</trd>
  <trd lng="ja">&#x9AA8;&#x7A81;&#x8D77;  <pr>&#12371;&#12388;&#12392;&#12387;&#x304D;</pr></trd>
  <trd lng="nl">apofyse</trd>
  <trd lng="pl">apofiza</trd>
  <trd lng="ru">&c_a;&c_p;&c_o;&c_f;&c_i;&c_z;</trd>
  <trdgrp lng="sk">
    <trd>apof&yacute;za</trd>,
    <trd>v&yacute;rastok<klr> (kosti)</klr></trd>,
    <trd>v&#xFD;be&#x17E;ok kosti</trd>,
    <trd>v&#xFD;rastok</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_r;&#x43E;&c_s;&c_t;&#x43E;&#x43A;</trd>,
    <trd>&c_v;&c_i;&c_s;&c_t;&c_u;&#x43F; &#x43A;&c_ib;&c_s;&c_t;&#x43A;&c_i;</trd>,
    <trd>&c_a;&#x43F;&#x43E;&c_f;&c_ib;&c_z;</trd>
  </trdgrp>
</drv>
