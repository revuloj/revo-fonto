<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>apofiz</rad>/o
</kap>
<drv mrk="apofiz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Osta elstara&jcirc;o:
      <ekz>
        akcesora, dorneca, frunta, tempia <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="diafiz.0o">diafizo</ref>,
      <ref cel="epifiz.0o">epifizo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Apophyse</trd>,
    <trd>Apophysis</trd>,
    <trd>sekund&auml;re Epiphyse</trd>,
    <trd>sekund&auml;re Epiphysis</trd>,
    <trd>Processus</trd>,
    <trd>Knochenfortsatz</trd>,
    <trd>vorspringender Knochenfortsatz</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>apophysis</trd>,
    <trd>process</trd>
  </trdgrp>
  <trd lng="fr">apophyse</trd>
  <trd lng="ru">&c_a;&c_p;&c_o;&c_f;&c_i;&c_z;</trd>
</drv>
</art>
