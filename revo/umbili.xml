<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>umbilik</rad>/o
</kap>
<drv mrk="umbili.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="umbili.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Ronda kaveto meze de ventro de mamuloj, produktita de cikatro de la
      <tld/>a &scirc;nuro.
    </dif>
    <trd lng="de">Nabel</trd>
    <trdgrp lng="fr">
      <trd>nombril</trd>,
      <trd>ombilic</trd>
    </trdgrp>
    <trd lng="nl">navel</trd>
    <trd lng="ru">&c_p;&c_u;&c_p;&c_o;&c_k;</trd>
  </snc>
  <snc mrk="umbili.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="dif" cel="hilum.0o">Hilumo</ref>.
    </dif>
    <trd lng="de">Nabelschnur</trd>
    <trd lng="ru">&c_r;&c_u;&c_b;&c_ch;&c_i;&c_k;</trd>
  </snc>
</drv>

<drv mrk="umbili.0asxnuro">
  <kap>
    <tld/>a &scirc;nuro
  </kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Membrana tubo, kuniganta feton kun placento.
    </dif>
  </snc>
  <trd lng="fr">cordon ombilical</trd>
  <trd lng="nl">navelstreng</trd>
  <trd lng="ru">&c_p;&c_u;&c_p;&c_o;&c_v;&c_i;&c_n;&c_a;</trd>
</drv>
</art>
