<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ceruz.xml,v 1.21 2023/10/14 11:10:02 revo Exp $">
<kap>
  <rad>ceruz</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ceruz.0o">
  <kap><tld/>o</kap>
  <snc mrk="ceruz.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Venena plumba karbonato kaj <ref tip="super" cel="hidroksid.0o">hidroksido</ref>,
      pasinte uzata en pentrado kaj farbado kiel blanka 
      <ref tip="lst" lst="voko:koloroj" cel="kolor.0ilo">kolorilo</ref>;
      simbole <frm>2 PbCO<sub>3</sub>, Pb(OH)<sub>2</sub></frm>:
      <ekz>
        <tld/>a blanko
        <fnt>
          <aut>trad. C.M.C.</aut>,
          <vrk><url ref="http://www.senlime.be/Kino/Perle.htm">
          La junulino kun la perlo</url></vrk>,
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_v;&c_ib;&c_n;&c_c;&c_o;&c_v;&c_a;&c_je; <ind>&c_b;&c_ja;&c_l;&c_ib;&c_l;&c_a;</ind></trd>
  <trd lng="cs">b&ecaron;loba olovnat&aacute;</trd>
  <trd lng="de">Bleiwei&szlig;</trd>
  <trd lng="en">white lead</trd>
  <trd lng="fa">&a_s;&a_f;&f_y;&a_d;&a_A;&a_b;</trd>
  <trd lng="fr">c&eacute;ruse</trd>
  <trd lng="hu">&oacute;lomfeh&eacute;r</trd>
  <trd lng="it">biacca</trd>
  <trd lng="ja">&#x925B;&#x767D;  <pr>&#x3048;&#x3093;&#x3071;&#x304F;</pr></trd>
  <trdgrp lng="nl">
    <trd>loodwit</trd>,
    <trd>kremserwit</trd>,
    <trd>schelpwit</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>cerusyt</trd>,
    <trd>w&eogonek;glan o&lstroke;owiu</trd>,
    <trd>biel o&lstroke;owiana</trd>,
    <trd>biel kremska</trd>,
    <trd>kit miniowy</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_v;&c_i;&c_n;&c_c;&c_o;&c_v;&c_y;&c_je; &c_b;&c_je;&c_l;&c_i;&c_l;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>PbCO2</trd>,
    <trd>olovnat&aacute; bieloba</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_v;&c_i;&#x43D;&c_c;&c_je;&c_v;&c_je; &c_b;&c_ib;&#x43B;&c_i;&#x43B;&#x43E;</trd>
</drv>
</art>
