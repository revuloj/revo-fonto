<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <rad>ceruz</rad>/o
  </kap>
  <drv mrk="ceruz.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">KEM</uzo>
    <snc>
      <dif>
        (2 PbCO<sub>3</sub>, Pb(OH)<sub>2</sub>). 
        Venena plumba karbonato kaj hidroksido, pasinte uzata en
        pentrado kaj farbado 
        kiel blanka kolorilo.
      </dif>
      <adm>
        La vorto "hidroksido" estas enkondukenda. <aut>MB</aut>
      </adm>
      <trd lng="de">Bleiwei&szlig;</trd>
      <trd lng="fa">&a_s;&a_f;&f_y;&a_d;&a_A;&a_b;</trd>
      <trd lng="fr">c&eacute;ruse</trd>
      <trd lng="hu">&oacute;lomfeh&eacute;r</trd>
      <trd lng="it">biacca</trd>
      <trdgrp lng="nl">
        <trd>loodwit</trd>,
        <trd>kremserwit</trd>,
        <trd>schelpwit</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>cerusyt</trd>,
        <trd>w&eogonek;glan o&lstroke;owiu</trd>,
        <trd>biel o&lstroke;owiana</trd>,
        <trd>biel kremska</trd>,
        <trd>kit miniowy</trd>
      </trdgrp>
      <trd lng="ru">&c_s;&c_v;&c_i;&c_n;&c_c;&c_o;&c_v;&c_y;&c_je;
      
      &c_b;&c_je;&c_l;&c_i;&c_l;&c_a;</trd>
    </snc>
  </drv>
</art>
