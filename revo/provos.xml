<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: provos.xml,v 1.22 2024/02/17 07:52:37 revo Exp $">
<kap>
  <rad>provos</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="provos.0o">
  <kap><tld/>o</kap>
  <snc mrk="provos.0o.JUR">
  <uzo tip="fak">JUR</uzo>
    <dif>
      <ref tip="super" cel="gard.0isto">Gardisto</ref> de 
      <ref tip="malprt" cel="liber.mal0ejo">malliberejo</ref>:
      <ekz>
        eniris unu <tld/>o kaj diris, ke mi havos viziton
        <fnt>
          <aut>Albert Camus, trad. Michel Duc Goninaz</aut>,
          <vrk>La Fremdulo</vrk>,
          <lok>S.A.T., 1993, p. 88</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam li estis enkarcerigita, la <tld/>oj forprenis de li liajn &scirc;ula&ccirc;ojn,
        po&scirc;tran&ccirc;ilon, kaj la zonon de lia pantalono, por eviti, ke li dama&gcirc;u sin
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2009/009680.php">Proksimaj kaj malproksimaj najbaroj</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> estas &gcirc;ismorte bastonita de dek <tld/>oj en sia &ccirc;elo
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2014/011290.php">En &ccirc;elo mortigita</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_r;&c_e;&c_m;&c_n;&c_y; &c_n;&c_a;&c_g;&c_l;&c_ja;&c_d;&c_ch;&c_y;&c_k;</trd>
  <trd lng="ca">escarceller</trd>
  <trdgrp lng="cs">
    <trd>profous</trd>,
    <trd>v&ecaron;ze&ncaron;sk&yacute; dozorce</trd>
  </trdgrp>
  <trd lng="de">Kerkermeister</trd>
  <trd lng="en">prison guard</trd>
  <trd lng="fa">&a_z;&a_n;&a_d;&a_A;&a_n;&a_b;&a_A;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>gardien de prison</trd>,
    <trd>ge&ocirc;lier</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x770B;&#x5B88;  <pr>&#x304B;&#x3093;&#x3057;&#x3085;</pr></trd>,
    <trd>&#x7262;&#x756A;  <pr>&#x308D;&#x3046;&#x3070;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">cipier</trd>
  <trdgrp lng="pl">
    <trd>profos</trd>,
    <trd>stra&zdot;nik  <klr>(wi&eogonek;zienny)</klr></trd>
  </trdgrp>
  <trd lng="pt">carcereiro</trd>
  <trd lng="ro">paznic de &icirc;nchisoare</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_ju;&c_r;&c_je;&c_m;&c_shch;&c_i;&c_k;</trd>,
    <trd>&c_t;&c_ju;&c_r;&c_je;&c_m;&c_n;&c_y;&c_j; &c_n;&c_a;&c_d;&c_z;&c_i;&c_r;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dozorca</trd>,
    <trd>&zcaron;al&aacute;rnik</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&#x44E;&c_r;&c_je;&#x43C;&#x43D;&c_i;&#x43A;</trd>,
    <trd>&c_t;&#x44E;&c_r;&c_je;&#x43C;&#x43D;&c_i;&c_j; &#x43D;&c_a;&c_g;&#x43B;&#x44F;&c_d;&c_a;&c_ch;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x72F1;&#x540F; <pr>y&#xF9;l&#xEC;</pr></trd>,
    <trd>&#x7344;&#x540F; <pr>y&#xF9;l&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>
