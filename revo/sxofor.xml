<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>&scirc;ofor</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sxofor.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxofor.0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      Profesia kondukisto de a&ubreve;to a&ubreve; de alia veturilo:
      <ekz>
        ili havas propran <tld/>on, dum veturpa&ubreve;zoj man&gcirc;as 
        en verdaj herbejoj
        <fnt><aut>W. Rohloff</aut>, <vrk>Mondoj</vrk>,
        <bib>Monato</bib>, <lok>jaro 2002a, numero 2a, p.
        31a</lok></fnt>;
      </ekz>
      <ekz>
        demandite pri la kialo, la bus<tld/>o respondis naive, ke oni
        faras tion intence &ccirc;ar rusoj militservas &ccirc;i tie
        <fnt><aut>B. Selimi</aut>, <vrk>Voja&gcirc;impresoj, somero
        2000</vrk>, <bib>Monato</bib>, <lok>jaro 2001a, numero 1a, p.
        26a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="auxt.0isto">a&ubreve;tisto</ref>,
      <ref cel="motor.0isto">motoristo</ref>,
      <ref cel="pilot.0o">piloto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_a;&c_f;&c_jo;&c_r;</trd>,
    <trd>&c_k;&c_ib;&c_r;&c_o;&c_w;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">xofer</trd>
  <trd lng="de">Fahrer</trd>
  <trdgrp lng="en">
    <trd>driver</trd>,
    <trd>chauffeur</trd>
  </trdgrp>
  <trd lng="es">ch&oacute;fer</trd>
  <trdgrp lng="fr">
    <trd>chauffeur<klr> (professionnel)</klr></trd>
  </trdgrp>
  <trd lng="he">&nun;&he;&gimel;<klr tip="amb"> 
(&bet;&mem;&qof;&tsadi;&vav;&ayin;&vav;)</klr></trd>
  <trdgrp lng="hu">
    <trd>g&eacute;pj&aacute;rm&udblac;vezet&odblac;</trd>,
    <trd>sof&odblac;r</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>autista<klr> (professionale)</klr></trd>,
    <trd>chauffeur</trd>, 
    <trd>conducente<klr> (autista professionista)</klr></trd>
  </trdgrp>
  <trd lng="nl">chauffeur</trd>
  <trdgrp lng="pt">
    <trd>motorista</trd>,
    <trd>chofer</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_o;&c_f;&c_jo;&c_r;</trd>
</drv>

<drv mrk="sxofor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxofor.0i.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      Profesie kondukadi veturilon:
      <ekz>
        oni devas atenti la vojon, kiam oni <tld/>as, &ccirc;u ne
        <fnt><bib>Gerda</bib>, <lok>&ccirc;apitro 13a</lok></fnt>?
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="en">chauffeur</trd>
  <trdgrp lng="fr">
    <trd>piloter<klr> (voiture, bus)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vezet<klr> (g&eacute;pj&aacute;rm&udblac;vet)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>guidare<klr> (come chauffeur o conducente)</klr></trd>,
    <trd>condurre<klr> (taxi, noleggi, bus)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="sxofor.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="sxofor.0ejo.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      Parto de <ref tip="malprt" cel="vetur.0ilo.TRA">veturilo</ref>,
      en kiu restas <tld/>o dum kondukado:
      <ekz>
        &leftquot;vi iru dormi en la kamiono&rightquot; li diris, 
        &leftquot;mi dormos en la <tld/>ejo&rightquot;
        <fnt>
          <aut>B. Felby</aut>,
          <vrk>
<url ref="http://members.fortunecity.co.uk/eskruco/034/u-04.html">
          Traviva&jcirc;oj de konata Kopenhagano</url></vrk>,
          <lok>Esperanto sub la Suda Kruco, 1998-07 &gcirc;is 08, vol. 6a,
          num. 4a (34)</lok>
        </fnt>
      </ekz> 
    </dif> 
  </snc>
  <trd lng="de">Fahrerkabine</trd>
  <trd lng="fr">cabine de pilotage</trd>
  <trd lng="it"><ind>cabina</ind> di guida</trd>
</drv>
</art>
