<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxofor.xml,v 1.37 2024/09/04 06:11:18 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>&scirc;ofor</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sxofor.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxofor.0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      Profesia kondukisto de a&ubreve;to a&ubreve; de alia veturilo:
      <ekz>
        ili havas propran <tld/>on, dum veturpa&ubreve;zoj man&gcirc;as
        en verdaj herbejoj
        <fnt><aut>W. Rohloff</aut>, <vrk>Mondoj</vrk>,
        <bib>Monato</bib>, <lok>jaro 2002a, numero 2a, p.
        31a</lok></fnt>;
      </ekz>
      <ekz>
        demandite pri la kialo, la bus<tld/>o respondis naive, ke oni
        faras tion intence &ccirc;ar rusoj militservas &ccirc;i tie
        <fnt><aut>B. Selimi</aut>, <vrk>Voja&gcirc;impresoj, somero
        2000</vrk>, <bib>Monato</bib>, <lok>jaro 2001a, numero 1a, p.
        26a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="auxt.0isto">a&ubreve;tisto</ref>,
      <ref cel="motor.0isto">motoristo</ref>,
      <ref cel="pilot.0o">piloto</ref>
    </refgrp>
    <ref tip="super" cel="vetur.0igisto">veturigisto</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_a;&c_f;&c_jo;&c_r;</trd>,
    <trd>&c_k;&c_ib;&c_r;&c_o;&c_w;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&#3905;&#3851;&#3939;&#3964;&#3851;&#3924;&#3851;</trd>
  <trd lng="ca">xofer</trd>
  <trd lng="de">Berufskraftfahrer (LKW-Fahrer, Busfahrer)</trd>
  <trdgrp lng="en">
    <trd>driver</trd>,
    <trd>chauffeur</trd>
  </trdgrp>
  <trd lng="es">ch&oacute;fer</trd>
  <trdgrp lng="fr">
    <trd>chauffeur<klr> (professionnel)</klr></trd>
  </trdgrp>
  <trd lng="he">&nun;&he;&#8503;<klr tip="amb">
(&bet;&mem;&qof;&tsadi;&vav;&ayin;&vav;)</klr></trd>
  <trdgrp lng="hu">
    <trd>g&eacute;pj&aacute;rm&udblac;vezet&odblac;</trd>,
    <trd>sof&odblac;r</trd>
  </trdgrp>
  <trd lng="id">sopir</trd>
  <trdgrp lng="it">
    <trd>autista<klr> (professionale)</klr></trd>,
    <trd>chauffeur</trd>,
    <trd>conducente<klr> (autista professionista)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#36939;&#36578;&#25163;  <pr>&#12358;&#12435;&#12390;&#12435;&#12375;&#12421;</pr></trd>
  <trd lng="nl">chauffeur <klr> (beroeps-)</klr></trd>
  <trdgrp lng="pl">
    <trd>kierowca</trd>,
    <trd>szofer</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>motorista</trd>,
    <trd>chofer</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_o;&c_f;&c_jo;&c_r;</trd>
  <trdgrp lng="uk">
    <trd>&c_sh;&c_o;&c_f;&c_je;&c_r;</trd>,
    <trd>&c_v;&c_o;&c_d;&c_ib;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#39381;&#39387;&#21729; <pr>ji&agrave;sh&#464;yu&aacute;n</pr></trd>,
    <trd>&#39550;&#39542;&#21592; <pr>ji&agrave;sh&#464;yu&aacute;n</pr></trd>,
    <trd>&#21496;&#26426; <pr>s&#299;j&#299;</pr></trd>,
    <trd>&#21496;&#27231; <pr>s&#299;j&#299;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxofor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxofor.0i.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      Profesie kondukadi veturilon:
      <ekz>
        oni devas atenti la vojon, kiam oni <tld/>as, &ccirc;u ne
        <fnt><bib>Gerda</bib>, <lok>&ccirc;apitro 13a</lok></fnt>?
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">conduir</trd>
  <trdgrp lng="de">
    <trd>kutschieren</trd>,
    <trd>fahren</trd>
    <trd>chauffieren [ark]</trd>,
    <trd>schoffieren [reg]</trd>
  </trdgrp>
  <trd lng="en">chauffeur</trd>
  <trdgrp lng="fr">
    <trd>piloter<klr> (voiture, bus)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vezet<klr> (g&eacute;pj&aacute;rm&udblac;vet)</klr></trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>kemudi</baz> mengemudikan</trd>,
    <trd><baz>kendara</baz> mengendarai</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>guidare<klr> (come chauffeur o conducente)</klr></trd>,
    <trd>condurre<klr> (taxi, noleggi, bus)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#36939;&#36578;&#12377;&#12427;  <pr>&#12358;&#12435;&#12390;&#12435;&#12377;&#12427;</pr></trd>
  <trd lng="nl">beroepshalve een voertuig <ind>besturen</ind></trd>
  <trdgrp lng="pl">
    <trd>prowdzi&cacute;  <ind>auto</ind></trd>,
    <trd>kierowa&cacute;  <ind>autem</ind></trd>
  </trdgrp>
</drv>

<drv mrk="sxofor.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="sxofor.0ejo.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      Parto de <ref tip="malprt" cel="vetur.0ilo.TRA">veturilo</ref>,
      en kiu restas <tld/>o dum kondukado:
      <ekz>
        &leftquot;vi iru dormi en la kamiono&rightquot; li diris,
        &leftquot;mi dormos en la <tld/>ejo&rightquot;
        <fnt>
          <aut>B. Felby</aut>,
          <vrk>
<url ref="http://members.fortunecity.co.uk/eskruco/034/u-04.html">
          Traviva&jcirc;oj de konata Kopenhagano</url></vrk>,
          <lok>Esperanto sub la Suda Kruco, 1998-07 &gcirc;is 08, vol. 6a,
          num. 4a (34)</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_je;&c_s;&c_c;&c_a; &c_k;&c_ib;&c_r;&c_o;&c_w;&c_c;&c_y;</trd>,
    <trd>&c_k;&c_a;&c_b;&c_ib;&c_n;&c_a; &c_k;&c_ib;&c_r;&c_o;&c_w;&c_c;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">cabina</trd>
  <trd lng="de">Fahrerkabine</trd>
  <trd lng="fr">cabine de pilotage</trd>
  <trd lng="id">kokpit</trd>
  <trd lng="it"><ind>cabina</ind> di guida</trd>
  <trdgrp lng="pl">
    <trd>kabina  <ind>kierowcy</ind></trd>,
    <trd>szoferka</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#39381;&#39387;&#23460; <pr>ji&agrave;sh&#464;sh&igrave;</pr></trd>,
    <trd>&#39550;&#39542;&#23460; <pr>ji&agrave;sh&#464;sh&igrave;</pr></trd>
  </trdgrp>
</drv>
</art>
