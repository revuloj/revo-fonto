<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>8</ofc> <rad>Polinezi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="poline.0o">
  <kap><tld/>o</kap>
  <snc mrk="poline.0o.SNC">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Vasta aro de insuloj en Pacifiko, inter A&ubreve;stralio kaj Ameriko.
    </dif>
  </snc>
  <trd lng="fr">Polyn&eacute;sie</trd>
</drv>

<drv mrk="poline.franca0o">
  <kap>Franca <tld/>o</kap>
  <snc mrk="poline.franca0o.SNC">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
     Grupo de kvin insularoj en Orienta Polinezio, sub kontrolo de
     Francio, kun propra registaro.
    </dif>
  </snc>
  <trd lng="fr">Polyn&eacute;sie fran&ccedil;aise</trd>
</drv>

<drv mrk="poline.0ano">
  <kap><tld lit="p"/>ano</kap>
  <snc mrk="poline.0ano.SNC">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
     Indi&gcirc;eno de <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">Polyn&eacute;sien</trd>
</drv>

</art>

<!--
$Log$
-->
</vortaro>