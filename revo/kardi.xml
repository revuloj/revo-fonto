<art mrk="$Id: kardi.xml,v 1.19 2024/03/06 06:29:59 revo Exp $">
<kap>
  <rad>kardi</rad>/o
</kap>

<drv mrk="kardi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Enlasa aperturo de la stomako.
    </dif>
  </snc>
  <rim>
    Pro la anatomia proksimeco la greka vorto &leftquot;<nac>k&alfa;&rho;&delta;&jota_ton;&alfa;</nac>&rightquot; resp. la latina
    &leftquot;<nac>cardia</nac>&rightquot;
    nomas kaj la enirejon de stomako kaj la koron. Pro tio la vorto trovi&gcirc;as kiel parto en
    multaj sciencaj korrilataj vortoj, vd <ref cel="kardiologi.0o">kardiologio</ref>.
  </rim>
  <trd lng="be">&c_k;&c_a;&c_r;&c_d;&c_y;&c_ja;</trd>
  <trdgrp lng="ca">
    <trd>c&agrave;rdia</trd>,
    <trd>c&agrave;rdies</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kardie</trd>,
    <trd>srdce</trd>,
    <trd>&ccaron;eslo</trd>
  </trdgrp>
  <trd lng="de">Cardia</trd>
  <trd lng="es">cardias</trd>
  <trdgrp lng="fa">
    <trd>&f_k;&a_A;&a_r;&a_d;&f_y;&a_A;</trd>,
    <trd>&a_d;&a_h;&a_A;&a_n;&a_h;&a_hamza_sure; &a_m;&a_ayn;&a_d;&a_h;</trd>,
    <trd>&a_f;&a_m;&#8204;&a_A;&a_l;&a_m;&a_ayn;&a_d;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">cardia</trd>
  <trd lng="hu">gyomorsz&aacute;j</trd>
  <trd lng="it">cardias</trd>
  <trd lng="ja">&#x30E8;&#x30FC;&#x30ED;&#x30C3;&#x30D1;&#x30B6;&#x30EB;&#x30AC;&#x30A4;</trd>
  <trd lng="nl">maagmond</trd>
  <trd lng="pl">wpust &zdot;o&lstroke;&aogonek;dka</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_r;&c_d;&c_i;&c_ja;</trd>,
    <trd>&c_v;&c_h;&c_o;&c_d; &c_v; &c_zh;&c_je;&c_l;&c_u;&c_d;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">srdcovka</trd>
  <trd lng="uk">&#x43A;&c_a;&c_r;&c_d;&c_ib;&#x43E;&#8230;</trd>
</drv>
</art>
