<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>bu&ccirc;</rad>/i
</kap>
<drv mrk="bucx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1">
    <dif>
      Mortigi brutojn por homa nutrado. 
    </dif>
    <trdgrp lng="fr">
      <trd>abattre <klr tip="ind">(un animal de boucherie)</klr></trd>,
      <trd>tuer <klr tip="ind">(un animal de boucherie)</klr></trd></trdgrp> 
    <trdgrp lng="ru">
      <trd>&c_u;&c_b;&c_i;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_s;&c_k;&c_o;&c_t;, &c_p;&c_t;&c_i;&c_c;&c_u;)</klr></trd>,
      <trd>&c_z;&c_a;&c_b;&c_i;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_s;&c_k;&c_o;&c_t;, &c_p;&c_t;&c_i;&c_c;&c_u;)</klr></trd>,
      <trd>&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol; <klr>(&c_s;&c_k;&c_o;&c_t;, &c_p;&c_t;&c_i;&c_c;&c_u;)</klr></trd>,
      <trd>&c_k;&c_o;&c_l;&c_o;&c_t;&c_mol; <klr>(&c_s;&c_k;&c_o;&c_t;, &c_p;&c_t;&c_i;&c_c;&c_u;)</klr></trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Amase a&ubreve; kruele mortigi:
      <ekz><tld/>as la lupo, oni anka&ubreve; &gcirc;in <tld/>os<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        milito estas <ind><tld/>ado</ind>, kiu senhonorigas la homaron;
        <trdgrp lng="fr">
          <trd>massacre</trd>,
          <trd>boucherie <klr tip="ind">(massacre)</klr></trd></trdgrp> 
      </ekz>
      <ekz>virina lango <tld/>as sen sango<fnt><bib>PrV</bib></fnt>.</ekz> 
    </dif>
    <trdgrp lng="fr">
      <trd>tuer <klr tip="ind">(massacrer)</klr></trd>,
      <trd>massacrer</trd></trdgrp> 
    <trd lng="ru">&c_v;&c_y;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol; <klr>(&c_n;&c_a;&c_s;&c_je;&c_l;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  </snc>
  <trdgrp lng="de"><trd>schlachten</trd>, <trd>abschlachten</trd></trdgrp>
  <trd lng="en">slaughter</trd>
</drv>
<drv mrk="bucx.0ejo">
  <kap><tld/>ejo</kap>
  <snc num="1">
    <dif>
      Loko kie oni <tld/>as la brutojn. 
    </dif>
    <trd lng="de">Schlachthof</trd>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Loko kie okazis kruela eksterma batalo.  
    </dif>
    <trd lng="de">Schlachtfeld</trd>
  </snc>
  <trd lng="fr">abattoir</trd>
  <trd lng="ru">&c_b;&c_o;&c_j;&c_n;&c_ja;</trd>
</drv>
</art>
