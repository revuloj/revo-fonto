<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lauxdaci.xml,v 1.9 2024/01/15 16:14:08 revo Exp $">
<kap>
  <rad>la&ubreve;daci</rad>/o
</kap>
<drv mrk="lauxdaci.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="lauxdaci.0o.BELE">
    <dif>
      <ref tip="dif" cel="parol.lauxd0o.BELE">La&ubreve;dparolo</ref>:
      <ekz>
        la <tld/>on digne faris d-ro Werner Bormann, prezidanto de la FAME-fonda&jcirc;o
        <klr>[&#x2026;]</klr>, kiu mem, simile al la premiito, okupis multajn poziciojn en
        la organizita Esperanto-movado
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 11</lok>
        </fnt>;
      </ekz>
      <ekz>
        en sia <tld/>o li prezentis la novajn <ref cel="lauxre.0o">la&ubreve;reato</ref>jn
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="&LOdE;Novaj/Nov10-33.htm">
          La 12a Esperanto-kulturpremio de Aalen</url></vrk>,
          <lok>La Ondo de Esperanto, 2010-11-12</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ditira.0o">ditirambo</ref>,
      <ref cel="panegi.0o">panegiro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_w;&c_d;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trd lng="de">Laudatio</trd>
  <trd lng="nl">laudatio</trd>
  <trd lng="fr">&eacute;loge</trd>
  <trd lng="pl">laudacja</trd>
  <trdgrp lng="zh">
    <trd>&#x8FFD;&#x60BC;&#x6587; <pr>zhu&#x12B;d&#xE0;ow&#xE9;n</pr></trd>,
    <trd>&#x980C;&#x8A5E; <pr>s&#xF2;ngc&#xED;</pr></trd>,
    <trd>&#x9882;&#x8BCD; <pr>s&#xF2;ngc&#xED;</pr></trd>,
    <trd>&#x8A84; <pr>l&#x11B;i</pr></trd>,
    <trd>&#x8BD4; <pr>l&#x11B;i</pr></trd>
  </trdgrp>
</drv>
</art>

