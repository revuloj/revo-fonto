<art mrk="$Id: ovr.xml,v 1.11 2022/03/24 15:32:04 revo Exp $">
<kap>
  <rad>ovr</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ovr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">POE</uzo>
  <snc mrk="ovr.0i.malfermi">
    <dif>
      <ref tip="dif" cel="ferm.mal0i">Malfermi</ref>:
      <ekz>
        <tld/>inte la okulojn, li vidis <klr>[&#x2026;]</klr> kadukan templon
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 66a, p. 428a</lok></fnt>;
      </ekz>
      <ekz>
        la lupo <klr>[&#x2026;]</klr> eksidis, <tld/>is lar&gcirc;e la fa&ubreve;kon
        <fnt>
          <vrk><url ref="https://lernu.net/cs/forumo/temo/23765/1">Porinfanaj ridigaj rakontoj / Humor a legrace / F&oacute;rum - lernu.net</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_k;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_ch;&c_y;&c_n;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">odkr&yacute;t</trd>
  <trdgrp lng="de">
    <trd>&ouml;ffnen</trd>,
    <trd>aufrei&szlig;en</trd>
  </trdgrp>
  <trdgrp lng="en"><trd>to ope (poetic)</trd><trd>open</trd></trdgrp>
  <trd lng="es">abrir</trd>
  <trd lng="fr">ouvrir</trd>
  <trdgrp lng="hu">
    <trd>nyit</trd>,
    <trd>kinyit</trd>,
    <trd>felt&aacute;r</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>openen<klr tip="ind"> (ontr.)</klr></trd>,
    <trd>opendoen<klr tip="ind"> (ontr.)</klr></trd>,
    <trd>openmaken<klr tip="ind"> (ontr.)</klr></trd>
  </trdgrp>
  <trd lng="pl">otwiera&cacute;</trd>
  <trd lng="pt">abrir</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_k;&c_r;&c_y;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_k;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">odkry&tcaron;</trd>
  <trd lng="tr">a&ccedil;mak</trd>
  <trdgrp lng="uk">
    <trd>&#x432;&#x456;&#x434;&#x43A;&#x440;&#x438;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x456;&#x434;&#x447;&#x438;&#x43D;&#x44F;&#x442;&#x438;</trd>
  </trdgrp>
</drv>
</art>

