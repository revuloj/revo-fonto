<vortaro>
<art mrk="$Id: makiav.xml,v 1.15 2020/05/03 14:10:21 revo Exp $">
<kap>
  <rad>makiavel</rad>/a
</kap>
<drv mrk="makiav.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Strebanta al potenco sen atento pri moralo,
      aganta sen skrupuloj:
      <ekz>
        &scirc;i komencis la abomenan efektivigon de <tld/>a plano, nome, iom
        post iom veneni
        la malfeli&ccirc;an izoli&gcirc;intan konkurantinon
        <fnt>
          <aut>Francisco C&acirc;ndido Xavier, trad. L. C. Porto Carreiro
          Neto</aut>
          <vrk><url
          ref="https://www.febnet.org.br/ba/file/livro%20em%20esperanto/Antauxdumiljaroj.pdf"
              >Anta&ubreve;&nbsp;du Mil Jaroj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kadre de kompleksa agadplano, sagaca a&ubreve; <tld/>a la&ubreve; la
        vidpunkto, la turka
        prezidanto <nom>Recep Tayyip Erdogan</nom> konsentis multigi la
        marajn patrolojn
        <fnt>
          <aut>Hans Kundnani , Astrid Ziebarth</aut>
          <vrk><url
          ref="http://neniammilitointerni.over-blog.com/2017/01/februaro-2017-le-monde-diplomatique-en-esperanto.html"
              >Inter Germanujo kaj Turkujo, la demando pri
              rifu&gcirc;intoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
    <adm>
      Aldoni &leftquot;Makiavelo&rightquot;, de kies nomo la vorto estas derivita
    </adm>
  <trdgrp lng="de">
    <trd>machiavellistisch</trd>,
    <trd>v&ouml;llig <ind>skrupellos</ind></trd>
  </trdgrp>
  <trd lng="be">&c_m;&c_a;&c_k;&c_ib;&c_ja;&c_v;&c_e;&c_l;&c_ib;&c_je;&c_w;&c_s;&c_k;&c_ib;</trd>
  <trd lng="ca">maquiav&egrave;l&middot;lic</trd>
  <trd lng="en">Machiavellian</trd>
  <trd lng="fr">maquiav&eacute;lique</trd>
  <trdgrp lng="hu">
    <trd>machiavellis</trd>,
    <trd>machiavellisztikus</trd>,
    <trd>g&aacute;tl&aacute;stalan</trd>
    <trd>k&iacute;m&eacute;letlen</trd>,
  </trdgrp>
  <trd lng="nl">machiavellistisch</trd>
  <trdgrp lng="pl">
    <trd>machiawelski</trd>,
    <trd>przebieg&lstroke;y</trd>,
    <trd>podst&eogonek;pny</trd>
  </trdgrp>
  <trd lng="pt">maquiav&eacute;lico</trd>
  <trdgrp lng="ro">
    <trd>viclean</trd>,
    <trd>insidios</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_a;&c_k;&c_i;&c_a;&c_v;&c_je;&c_l;&c_l;&c_i;&c_je;&c_v;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
