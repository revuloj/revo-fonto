<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aranee.xml,v 1.10 2023/04/03 08:31:20 revo Exp $">
<kap>
  <rad>araneed</rad>/oj
</kap>
<drv mrk="aranee.0oj">
  <kap><tld/>oj</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt" cel="arane.0uloj">araneuloj</ref>
      (<trd lng="la">Araneidae</trd>):
      <ekz>
        <tld/>oj havas ok similajn okulojn, harecajn a&ubreve; spinecajn krurojn, kaj ne havas
        sonproduktajn organojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=378">Araneedoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_v;&c_u;&c_k;&c_ib;-&c_k;&c_r;&c_u;&c_g;&c_a;&c_p;&c_r;&c_a;&c_d;&c_y;</trd>
  <trd lng="de">Radnetzspinnen</trd>
  <trd lng="fr">Araneidae</trd>
  <trd lng="he">&#x5E2;&#x5DB;&#x5D1;&#x5D9;&#x5E9;&#x5D9;&#x5DD;</trd>
  <trd lng="hu">p&oacute;kf&eacute;l&eacute;k</trd>
  <trd lng="nl">wielspinnen</trd>
  <trd lng="ru">&c_p;&c_a;&c_u;&c_k;&c_i;-&c_k;&c_r;&c_u;&c_g;&c_o;&c_p;&c_r;&c_ja;&c_d;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>

