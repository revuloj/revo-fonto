<kap>
  <rad>gramine</rad>/oj
</kap>
<drv mrk="gramine.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Tradicia nomo de <ref tip="dif" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Gramineae</trd>):
      <ekz>
        stepo estas herba, senarba vegeta&jcirc;aro de la mezvarmaj a&ubreve; malvarmaj, sekaj
        regionoj, karakterizata de <klr>[&#8230;]</klr> <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=50344">Stepo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_l;&c_a;&c_k;&c_ib; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  <trd lng="ca">gram&iacute;nies</trd>
  <trdgrp lng="cs">
    <trd>traviny</trd>,
    <trd>tr&aacute;vy</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gramineen</trd>,
    <trd>Gr&auml;ser</trd>
  </trdgrp>
  <trd lng="fr">gramin&eacute;es</trd>
  <trd lng="hu">p&aacute;zsitf&udblac;f&eacute;l&eacute;k</trd>
  <trd lng="ja">&#x30A4;&#x30CD;&#x79D1;  <pr>&#x30A4;&#x30CD;&#x304B;</pr></trd>
  <trd lng="nl">echte <ind>grassen</ind><klr tip="ind"> (fam.)</klr></trd>
  <trdgrp lng="pl">
    <trd>wiechlinowate</trd>,
    <trd>trawy</trd>
  </trdgrp>
  <trd lng="pt">gram&iacute;neas</trd>
  <trd lng="ru">&c_z;&c_l;&c_a;&c_k;&c_i;</trd>
  <trdgrp lng="sk">
    <trd>traviny</trd>,
    <trd>tr&aacute;vy</trd>
  </trdgrp>
  <trd lng="uk">&c_z;&#x43B;&c_a;&#x43A;&#x43E;&c_v;&c_ib;</trd>
  <trd lng="zh">&#x79BE;&#x672C;&#x79D1;&#x690D;&#29289; <pr>h&#xE9;b&#x11B;nk&emacron;zh&#xED;w&#xF9;</pr></trd>
</drv>
