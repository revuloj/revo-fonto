<art mrk="$Id: gramine.xml,v 1.13 2022/03/19 19:18:23 revo Exp $">
<kap>
  <rad>gramine</rad>/oj
</kap>
<drv mrk="gramine.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Tradicia nomo de <ref tip="dif" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Gramineae</trd>):
      <ekz>
        stepo estas herba, senarba vegeta&jcirc;aro de la mezvarmaj a&ubreve; malvarmaj, sekaj
        regionoj, karakterizata de <klr>[&#x2026;]</klr> <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=50344">Stepo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">gram&iacute;nies</trd>
  <trdgrp lng="cs">
    <trd>traviny</trd>,
    <trd>tr&aacute;vy</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gramineen</trd>,
    <trd>Gr&auml;ser</trd>
  </trdgrp>
  <trd lng="fr">gramin&eacute;es</trd>
  <trd lng="hu">p&aacute;zsitf&udblac;f&eacute;l&eacute;k</trd>
  <trd lng="nl">echte <ind>grassen</ind><klr tip="ind">
  (fam.)</klr></trd>
  <trd lng="pt">gram&iacute;neas</trd>
  <trd lng="ru">&c_z;&c_l;&c_a;&c_k;&c_i;</trd>
  <trdgrp lng="sk">
    <trd>traviny</trd>,
    <trd>tr&aacute;vy</trd>
  </trdgrp>
  <trd lng="uk">&#x437;&#x43B;&#x430;&#x43A;&#x43E;&#x432;&#x456;</trd>
</drv>
</art>
