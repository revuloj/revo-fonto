<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: anselm.xml,v 1.8 2023/08/14 12:43:32 revo Exp $">
<kap>
  <rad>Anselm</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="anselm.0o">
  <kap><tld/>o</kap>
  <snc mrk="anselm.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        mi estas tre humila servantino de sinjoro <tld/>o
        <fnt>
          <aut>Moli&egrave;re, trad. Samuel Meyer</aut>,
          <vrk><url ref="http://www.luin.se/inko/198-4.pdf">La
avarulo</url></vrk>,
          <lok>1904</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o kaj <nom>Elvira</nom>
        <klr>(poemaro de <nom>Reino Eriksson</nom>)</klr>
        <fnt>
          <aut>Reino Eriksson</aut>,
          <vrk>Anselmo kaj Elvira</vrk>,
          <lok>Iisalmi: Altarte, 1999</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_A;&c_n;&c_s;&c_e;&c_l;&c_mol;&c_m;</trd>
  <trd lng="de">Anselm</trd>
  <trd lng="en">Anselm</trd>
  <trd lng="fi">Anselmi</trd>
  <trd lng="fr">Anselme</trd>
  <trd lng="it">Anselmo</trd>
  <trd lng="pl">Anzelm</trd>
  <trd lng="ru">&c_A;&c_n;&c_s;&c_je;&c_l;&c_mol;&c_m;</trd>
</drv>
</art>

