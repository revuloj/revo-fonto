<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>kunikl</rad>/o
</kap>
<drv mrk="kunikl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="lepor.0edoj">leporedoj</ref>
      (<trd lng="la">Oryctolagus</trd>) kun mallongaj postaj kruroj, kaj
      bredata pro karno kaj felo.
    </dif>
  </snc>
  <trd lng="de">Kaninchen</trd>
  <trd lng="fr">lapin</trd>
</drv>
</art>
