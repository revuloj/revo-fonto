<vortaro>
  <art mrk="$Id: dalmati.xml,v 1.7 2023/08/31 10:43:57 revo Exp $">
    <kap>
      <rad>Dalmati</rad>/o
    </kap>

<drv mrk="dalmati.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:regionoj" cel="region.0o.land">Regiono</ref>
      &ccirc;e la orienta bordo de <ref cel="adriatik.0o">Adriatiko</ref> en la sudo de
      <ref tip="malprt" cel="kroat.K0ujo">Kroatujo</ref>:
      <ekz>
        la iliroj vivis anta&ubreve; &ccirc;irka&ubreve; 2400 jaroj krom en <tld/>o anka&ubreve; en norda kaj
        centra Albanio kaj vaste landinterne
        <fnt>
	<aut>Uwe Joachim Moritz/Aldo de' Giorgi</aut>
	<vrk><url ref="&Monato;/2000/007043.php">Du opinioj</url></vrk>
	<bib>Monato</bib>
	<lok>2000/07, p. 4</lok>
        </fnt>;
      </ekz>
      <ekz>
        nia afablega &ccirc;i&ccirc;erono kaj interpretisto en <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2003/008020.html">Man&gcirc;u por sano,
          samideano!</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la Kornatoj situas anta&ubreve; la marbordo de centra <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=29174">Adriatiko</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Dalmatien</trd>
  </snc>
  <trd lng="be">&c_D;&c_a;&c_l;&c_m;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trd lng="ja">&#x30C0;&#x30EB;&#x30DE;&#x30C6;&#x30A3;&#x30A2;</trd>
  <trd lng="uk">&c_D;&c_a;&#x43B;&#x43C;&c_a;&c_t;&c_ib;&#x44F;</trd>
</drv>

