<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>endomorfi</rad>/o
</kap>
<drv mrk="endomo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="homomo.0o">Homomorfio</ref> de iu aro al 
      &gcirc;i mem:
      <ekz>
        vektora <tld/>o <klr>(super vektora spaco)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      Atributoj de endomorfio:
      <ref cel="bild.0aro.MAT">bildaro</ref>,
      <ref cel="kern.0o.homomorfio">kerno</ref>;
      Atributoj kaj epitetoj por vektora endomorfio:
      <ref cel="ajgen.0o">ajgeno</ref>,
      <ref cel="ajgen.0subspaco">ajgensubspaco</ref>,
      <ref cel="ajgen.0vektoro">ajgenvektoro</ref>,
      <ref cel="determ.0anto.endomorfio">determinanto</ref>,
      <ref cel="diagon.0igebla.endomorfio">diagonaligebla</ref>,
      <ref cel="polino.karakteriza0o">karakteriza polinomo</ref>,
      <ref cel="matric.0o.homomorfio">matrico</ref>,
      <ref cel="rang.0o.homomorfio">rango</ref>,
      <ref cel="spektr.0o.ajgeno">spektro</ref>;
    </refgrp>
    <refgrp tip="sub">
      Ekzemploj de vektoraj endomorfioj:
      <ref cel="projek.0o.vektora">projekcio</ref>,
      <ref cel="rotaci.0o.vektora">rotacio</ref>,
      <ref cel="simetr.0o.vektora">simetrio</ref>,
      <ref cel="simil.0eco.vektora">simileco</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Endomorphismus</trd>
  <trd lng="en">endomorphism</trd>
  <trd lng="fr">endomorphisme</trd>
  <trd lng="ru">&c_e;&c_n;&c_d;&c_o;&c_m;&c_o;&c_r;&c_f;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
