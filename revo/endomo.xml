<vortaro>
<art mrk="$Id$">
<kap>
<rad>endomorfi</rad>/o
</kap>
<drv mrk="endomo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="homomo.0o">Homomorfio</ref> de iu
      aro al si mem.
    </dif>
    <refgrp tip="vid">
      Atributoj:
      <ref cel="ajgen.0o">ajgeno</ref>,
      <ref cel="ajgen.0subspaco">ajgensubspaco</ref>,
      <ref cel="spektr.0o.ajgeno">spektro</ref>.
    </refgrp>
  </snc>
  <trd lng="fr">endomorphisme</trd>
  <trd
  lng="ru">&c_e;&c_n;&c_d;&c_o;&c_m;&c_o;&c_r;&c_f;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
