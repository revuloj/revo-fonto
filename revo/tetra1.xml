<vortaro>
  <art mrk="$Id: tetra1.xml,v 1.3 2026/02/12 17:15:58 afido Exp $">
    <kap>
      <rad>tetra</rad>-,
      <var><kap><rad var="r">tetr</rad>-</kap></var>
    </kap>

<drv mrk="tetra1.0">
  <kap><tld/>-, <var><kap><tld var="r"/>-</kap></var></kap>
  <snc mrk="tetra1.0.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Neoficiala scienca prefikso, signifanta &leftquot;kvar-&rightquot;
      (da funkcia grupo)
      en nomoj de &hcirc;emia&jcirc;oj la&ubreve; internacia nomenklaturo
      <ref cel="uni.Internacia_0_de_Teoria_kaj_Aplika_Kemio">IUTAK</ref>:
      <ekz>
        pulvoro de osmio <klr>[&#8230;]</klr> transformi&gcirc;as en vapori&gcirc;eman <klr>[&#8230;]</klr> <tld
        var="r"/>oksidon <frm>OsO<sub>4</sub></frm>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=25717"
              >Osmio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        2,2,4,4-<tld/>metila-3-pentanono
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=866979"
              >Du-t-butila ketono</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">tetra-</trd>
    <trd lng="en">tetra-</trd>
    <trd lng="fr">tetra-</trd>
  </snc>
</drv>

  </art>

