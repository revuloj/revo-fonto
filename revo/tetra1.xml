<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: tetra1.xml,v 1.1 2026/02/10 07:21:23 afido Exp $">
    <kap>
      <rad>tetra</rad>-,
      <var><kap><rad var="r">tetr</rad>-</kap></var>
    </kap>

<drv mrk="mono.0">
  <kap><tld/>-, <var><kap><tld var="r"/>-</kap></var></kap>
  <snc mrk="mono.0.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Neoficiala scienca prefikso, signifanta &leftquot;kvar-&rightquot;
      (da funkcia grupo)
      en nomoj de &hcirc;emia&jcirc;oj la&ubreve; internacia nomenklaturo <nom>IUPAC</nom>:
      <ekz>
        pulvoro de osmio <klr>[&#x2026;]</klr> transformi&gcirc;as en vapori&gcirc;eman <klr>[&#x2026;]</klr> <tld
        var="r"/>oksidon <frm>OsO<sub>4</sub></frm>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=25717"
              >Osmio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        2,2,4,4-<tld/>metila-3-pentanono
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=866979"
              >Du-t-butila ketono</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">tetra-</trd>
    <trd lng="en">tetra-</trd>
    <trd lng="fr">tetra-</trd>
  </snc>
</drv>

  </art>

