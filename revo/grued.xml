<kap>
  <rad>grued</rad>/oj
</kap>
<drv mrk="grued.0oj">
  <kap>
    <tld/>oj
  </kap>
  <snc mrk="grued.0oj.ZOO">
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="gru.0formaj">gruformaj
      birdoj</ref> (<trd lng="la">Gruidae</trd>):
      <ekz>
        <ref tip="prt" cel="gru.0o.ZOO">gruo</ref>
        estas genro de migrantaj birdoj el la <klr>[&#8230;]</klr> familio de <tld lit="G"/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=47208">Gruo (birdo)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_u;&c_r;&c_a;&c_w;&c_l;&c_ib;&c_n;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  <trd lng="ca">gruids</trd>
  <trd lng="de">Kraniche<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="en">cranes<klr tip="ind"> (family)</klr></trd>
  <trd lng="fr">gruid&eacute;s</trd>
  <trd lng="hu">daruf&eacute;l&eacute;k</trd>
  <trd lng="nl">kraanvogels<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="ru">&c_zh;&c_u;&c_r;&c_a;&c_v;&c_l;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="pt">gru&iacute;deos</trd>
  <trd lng="zh">&#x9DB4;&#x79D1; <pr>h&#xE8;k&emacron;</pr></trd>
</drv>
