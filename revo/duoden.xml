<drv mrk="duoden.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Parto de la <ref tip="malprt" cel="intest.maldika0o">maldika intesto</ref>
      trovi&gcirc;anta malanta&ubreve; la stomako:
      <ekz>
        la <tld/>o estas la sola parto de la intesto, kiu ne partoprenas en la
        peristalta movi&gcirc;o, la pH-valoro en la <tld/>o estas inter 6,0 kaj 6,5, do
        iomete acida
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=843">Duodeno</url></lok>
        </fnt>.
      </ekz>  
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_v;&c_a;&c_n;&c_a;&c_c;&c_c;&c_a;&c_c;&c_ib;&c_p;&c_a;&c_l;&c_mol;&c_c;&c_a;&c_v;&c_a;&c_ja; <ind>&c_k;&c_ib;&c_sh;&c_k;&c_a;</ind></trd>
  <trd lng="bg">&c_d;&c_v;&c_a;&c_n;&c_a;&c_d;&c_je;&c_s;&c_je;&c_t;&c_o;&c_p;&c_r;&c_malmol;&c_s;&c_t;&c_n;&c_i;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>duodenum</trd>,
    <trd>dvan&aacute;ctern&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Zw&ouml;lffingerdarm</trd>,
    <trd>Duodenum</trd>
  </trdgrp>
  <trd lng="en">duodenum</trd>
  <trd lng="es">duodeno</trd>
  <trdgrp lng="fa">
    <trd>&a_d;&a_w;&a_A;&a_z;&a_d;&a_h;&a_h;</trd>,
    <trd>&a_A;&a_th;&a_n;&f_y;&zwnj;&a_ayn;&a_sh;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">duod&eacute;num</trd>
  <trd lng="he">&tav;&resh;&yod;&samekh;&resh;&yod;&vav;&fnun;</trd>
  <trdgrp lng="hu">
    <trd>nyomb&eacute;l</trd>,
    <trd>duodenum</trd>
  </trdgrp>
  <trd lng="it">duodeno</trd>
  <trd lng="ja">&#x5341;&#x4E8C;&#x6307;&#x8178;  <pr>&#x3058;&#x3085;&#x3046;&#x306B;&#x3057;&#x3061;&#x3087;&#x3046;</pr></trd>
  <trdgrp lng="nl">
    <trd>twaalfvingerige darm</trd>,
    <trd>duodenum</trd>
  </trdgrp>
  <trd lng="pl">dwunastnica</trd>
  <trd lng="pt">duodeno</trd>
  <trd lng="ru">&c_d;&c_v;&c_je;&c_n;&c_a;&c_d;&c_c;&c_a;&c_t;&c_i;&c_p;&c_je;&c_r;&c_s;&c_t;&c_n;&c_a;&c_ja; &c_k;&c_i;&c_sh;&c_k;&c_a;</trd>
  <trd lng="sk">dvan&aacute;stnik</trd>
  <trd lng="uk">&#x434;&#x432;&#x430;&#x43D;&#x430;&#x434;&#x446;&#x44F;&#x442;&#x438;&#x43F;&#x430;&#x43B;&#x430; &#x43A;&#x438;&#x448;&#x43A;&#x430;</trd>
  <trd lng="zh">&#x5341;&#x4E8C;&#x6307;&#x80A0; <pr>sh&#xED;&#xE8;rzh&#x1D0;ch&#xE1;ng</pr></trd>
