<art mrk="$Id: solist.xml,v 1.20 2022/05/05 14:43:48 revo Exp $">
<kap>
   <ofc>1</ofc>
   <rad>solist</rad>/o
</kap>
<drv mrk="solist.0o">
  <kap><tld/>o</kap>
  <snc mrk="solist.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="muzik.0isto">Muzikisto</ref>
      a&ubreve; <ref tip="super" cel="kant.0isto.profesio">kantisto</ref>, kiu ludas sole:
      <ekz>
        la nove preparitan a&ubreve;toran version prezentis <tld/>oj de la moskva teatro
        <nom>Gelikon-opera</nom> en la Moskva Internacia Muzikdomo
        <fnt>
          <bib>Monato</bib>
          <aut>Anna kaj Mati Pentus</aut>
          <vrk><url ref="&Monato;/2011/010539.php">Princo Igor neniam a&ubreve;dita</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;Gor&gcirc;o-n 2000&rightquot; partoprenis kantistoj, operaj <tld/>oj kaj futbal&scirc;atantoj
        <fnt>
          <bib>Monato</bib>
          <aut>LAST</aut>
          <lok><url ref="&Steloj;monato2/006954.html">&Steloj;monato2/006954.html</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_l;&c_ib;&c_s;&c_t;</trd>
  <trd lng="ca">solista</trd>
  <trd lng="cs">s&oacute;lista</trd>
  <trd lng="de">Solist</trd>
  <trd lng="en">soloist</trd>
  <trd lng="es">solista</trd>
  <trd lng="fr">soliste</trd>
  <trdgrp lng="hu">
     <trd>sz&oacute;lista</trd>,
     <trd>sz&oacute;l&oacute;j&aacute;t&eacute;kos</trd>,
     <trd>sz&oacute;l&oacute;&eacute;nekes</trd>
   </trdgrp>
  <trd lng="it">solista</trd>
  <trd lng="nl">solist</trd>
  <trd lng="ru">&c_s;&c_o;&c_l;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">s&oacute;lista</trd>
  <trd lng="uk">&#x441;&#x43E;&#x43B;&#x456;&#x441;&#x442;</trd>
</drv>
</art>
