    <trd>&#x6297;&#x8861; <pr>k&#xE0;ngh&#xE9;ng</pr></trd>,
    <trd>&#x51FA;&#x8CFD; <pr>ch&#x16B;s&#xE0;i</pr></trd>,
    <trd>&#x7ADE;&#x4E89;&#x6027; <pr>j&#xEC;ngzh&#x113;ngx&#xEC;ng</pr></trd>,
