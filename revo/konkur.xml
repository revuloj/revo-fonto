<kap>
  <ofc>*</ofc>
  <rad>konkur</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="konkur.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="konkur.0i.KOMUNE">
    <dif>
      Vetbatali kontra&ubreve; alia por atingi anta&ubreve; li difinitan
      celon; peni por superi alian:
      <ekz>
        <tld/>i iun, kun iu per talento, klopodoj;
      </ekz>
      <ekz>
        <tld/>i kontra&ubreve; iu por akiri ies favoron;
      </ekz>
      <ekz>
        <tld/>i kun iu pri vakanta ofico;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>antaj interesoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la muziko pene <tld/>adis kun la rapida dancado<fnt>Z</fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_a;&c_b;&c_o;&c_r;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_u;&c_p;&c_je;&c_r;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_k;&c_u;&c_r;&c_y;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_s;&c_malmol;&c_s;&c_t;&c_je;&c_z;&c_a;&c_v;&c_a;&c_m; &c_s;&c_je;</trd>,
    <trd>&c_s;&c_malmol;&c_r;&c_je;&c_v;&c_n;&c_o;&c_v;&c_a;&c_v;&c_a;&c_m; &c_s;&c_je;</trd>
  </trdgrp>
  <trd lng="ca">competir</trd>
  <trdgrp lng="cs">
    <trd>sout&ecaron;&zcaron;it</trd>,
    <trd>z&aacute;vodit</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>konkurrieren</trd>,
     <trd>wetteifern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>compete</trd>,
    <trd>contend</trd>,
    <trd>rival</trd>
  </trdgrp>
  <trd lng="es">competir</trd>
  <trdgrp lng="fr">
     <trd>rivaliser</trd>,
     <trd>concurrencer</trd>,
     <trd>faire concurrence &agrave;</trd>
  </trdgrp>
  <trd lng="nl">wedijveren</trd>
  <trdgrp lng="pl">
    <trd>konkurowa&cacute;</trd>,
    <trd>rywalizowa&cacute;</trd>,
    <trd>wsp&oacute;&lstroke;zawodniczy&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>concorrer</trd>,
    <trd>competir</trd>,
    <trd>rivalizar</trd>,
    <trd>porfiar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_r;&c_je;&c_v;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_o;&c_p;&c_je;&c_r;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">s&uacute;&tcaron;a&zcaron;i&tcaron;</trd>
  <trd lng="tp">utala</trd>
</drv>

<drv mrk="konkur.0ado">
  <kap><tld/>o, <var><kap><tld/>ado</kap></var></kap>
  <snc mrk="konkur.0ado.KOMUNE">
    <dif>
      Vetbatalo inter pluraj personoj:
      <ekz>
        lojala <tld/>o<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>o pri profito;
      </ekz>
      <ekz>
        <tld/>ado al feli&ccirc;o, en militpreparoj;
      </ekz>
      <ekz>
        la ka&ubreve;zo de la intergenta malamo ne estas la <tld/>ado
        ekonomia<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konkur1.0o">konkurenco</ref>,
      <ref cel="konkur2.0o">konkurso</ref>
    </refgrp>
  </snc>
  <trd lng="af">ras</trd>
  <trd lng="am">&#x12D8;&#x122D;</trd>
  <trd lng="ar">&#x633;&#x628;&#x627;&#x642;</trd>
  <trd lng="az">irqi</trd>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_a;&c_b;&c_o;&c_r;&c_n;&c_ib;&c_c;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_p;&c_je;&c_r;&c_n;&c_ib;&c_c;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_k;&c_u;&c_r;&c_e;&c_n;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_s;&c_malmol;&c_s;&c_t;&c_je;&c_z;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_malmol;&c_r;&c_je;&c_v;&c_n;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_malmol;&c_p;&c_je;&c_r;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="bn">&#x99C;&#x9BE;&#x9A4;&#x9BF;</trd>
  <trd lng="bo">&#xF60;&#xF42;&#xFB2;&#xF53;&#xF0B;&#xF66;&#xFA1;&#xF74;&#xF62;&#xF0B;</trd>
  <trd lng="bs">rase</trd>
  <trd lng="ca">competici&oacute;</trd>
  <trd lng="co">a punti</trd>
  <trdgrp lng="cs">
    <trd>hospod&aacute;&rcaron;sk&aacute; sout&ecaron;&zcaron;</trd>,
    <trd>rivalita</trd>,
    <trd>sout&ecaron;&zcaron;en&iacute;</trd>
  </trdgrp>
  <trd lng="da">l&#xF8;b</trd>
