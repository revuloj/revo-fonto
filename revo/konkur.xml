  <trd lng="en">competition</trd>
  <trd lng="es">competici&oacute;n</trd>
  <trd lng="et">rassi</trd>
  <trd lng="eu">lasterketa</trd>
  <trdgrp lng="fr">
    <trd>concours</trd>,
    <trd>concurrence</trd>,
    <trd>comp&eacute;tition</trd>,
    <trd>rivalit&eacute;</trd>
  </trdgrp>
  <trd lng="fy">ras</trd>
  <trd lng="gl">carreira</trd>
  <trd lng="gu">&#xAB0;&#xAC7;&#xAB8;</trd>
  <trd lng="hi">&#x926;&#x94C;&#x921;&#x93C;</trd>
  <trd lng="hr">utrka</trd>
  <trd lng="hy">&#x574;&#x580;&#x581;&#x561;&#x57E;&#x561;&#x566;&#x584;</trd>
  <trd lng="is">kapp</trd>
  <trd lng="ja">&#x30EC;&#x30FC;&#x30B9;</trd>
  <trd lng="jw">lomba</trd>
  <trd lng="ka">&#x10E0;&#x10D0;&#x10E1;&#x10D8;&#x10E1;</trd>
  <trd lng="km">&#x1780;&#x17B6;&#x179A;&#x1794;&#x17D2;&#x179A;&#x178E;&#x17B6;&#x17C6;&#x1784;</trd>
  <trd lng="ko">&#xB808;&#xC774;&#xC2A4;</trd>
  <trd lng="lo">&#xEC0;&#xE8A;&#xEB7;&#xEC9;&#xEAD;&#xE8A;&#xEB2;&#xE94;</trd>
