<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>konkur</rad>/i
</kap>
<drv mrk="konkur.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Vetbatali kontra&ubreve; alia por atingi anta&ubreve; li difinitan
      celon; peni por superi alian:
      <ekz>
        <tld/>i iun, kun iu per talento, klopodoj;
      </ekz>
      <ekz>
        <tld/>i kontra&ubreve; iu por akiri ies favoron;
      </ekz>
      <ekz>
        <tld/>i kun iu pri vakanta ofico;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>antaj interesoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la muziko pene <tld/>adis kun la rapida dancado<fnt>Z</fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>konkurrieren</trd>,
     <trd>wetteifern</trd>
  </trdgrp>
  <trd lng="en">compete</trd>
  <trd lng="nl">wedijveren</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_r;&c_je;&c_v;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_o;&c_p;&c_je;&c_r;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="konkur.0ado">
  <kap><tld/>(ad)o</kap>
  <snc>
    <dif>
      Vetbatalo inter pluraj personoj:
      <ekz>
        lojala <tld/>o<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>o pri profito;
      </ekz>
      <ekz>
        <tld/>ado al feli&ccirc;o, en militpreparoj;
      </ekz>
      <ekz>
        la ka&ubreve;zo de la intergenta malamo ne estas la <tld/>ado
        ekonomia<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Wettbewerb</trd>,
     <trd>Wettstreit</trd>,
     <trd>Wettlauf</trd>
  </trdgrp>
  <trd lng="nl">wedijver</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_r;&c_je;&c_v;&c_n;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_o;&c_p;&c_je;&c_r;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="konkur.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Kiel <tld/>antoj; vete:
      <ekz>
        ili <tld/>e paroladis;
      </ekz>
      <ekz>
        &ccirc;iuj <tld/>e rapidas al la pordo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>um die Wette</trd>
  </trdgrp>
  <trd lng="nl">om het <ind>hardst</ind></trd>
  <trd lng="ru">&c_n;&c_a;&c_p;&c_je;&c_r;&c_je;&c_g;&c_o;&c_n;&c_k;&c_i;</trd>
</drv>
</art>
