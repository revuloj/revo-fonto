     <trd>concurrencer</trd>,
     <trd>faire concurrence &agrave;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7AF6;&#x4E89;&#x3059;&#x308B;  <pr>&#x304D;&#x3087;&#x3046;&#x305D;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x7AF6;&#x3046;  <pr>&#x304D;&#x305D;&#x3046;</pr></trd>,
    <trd>&#x5F35;&#x308A;&#x5408;&#x3046;  <pr>&#x306F;&#x308A;&#x3042;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">wedijveren</trd>
  <trdgrp lng="pl">
