<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>fortun</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="fortun.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Diino de la &scirc;anco. 
    </dif>
    <trd lng="de">Fortuna</trd>
    <trd lng="fr">Fortune</trd>
    <trd lng="hu">Fortuna</trd>
    <trd lng="ru">&c_F;&c_o;&c_r;&c_t;&c_u;&c_n;&c_a;</trd>
  </snc>
  <snc num="2">
    <dif>
      Bona sorto. 
    </dif>
    <refgrp tip="vid">
      <ref cel="destin.0o">destino</ref>,
      <ref cel="hazard.0o">hazardo</ref>,
      <ref cel="fat.0o">fato</ref>.
    </refgrp>
    <trdgrp lng="de">
     <trd>Gl&uuml;ck</trd>,
     <trd>Gunst des Schicksals</trd></trdgrp>
    <trd lng="es">fortuna</trd>
    <trdgrp lng="fr">
      <trd>fortune<klr tip="ind"> (chance)</klr></trd>,
      <trd>chance</trd></trdgrp> 
    <trdgrp lng="hu">
      <trd>szerencse</trd>,
      <trd>j&oacute; <ind>sors</ind></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_f;&c_o;&c_r;&c_t;&c_u;&c_n;&c_a;</trd>,
      <trd>&c_v;&c_je;&c_z;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
</drv>
</art>
