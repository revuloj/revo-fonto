<art mrk="$Id$">
<kap>
  <rad>empir</rad>/o
</kap>
<drv mrk="empir.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">BELA</uzo>
    <dif>
      Franca ar&hcirc;itektura, dekoracia kaj mebla stilo, prospera precipe
      dum la regado de Napoleono la 1a, proksimume 1800&ndash;1815,
      strikte imita de grek-romiaj monument-planoj, libere reuzanta
antikvajn
      romiajn kaj egiptajn ornama&jcirc;ojn.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_p;&c_ib;&c_r;</trd>
  <trd lng="de">Empire(stil)</trd>
  <trd lng="fr">style <ind>empire</ind></trd>
  <trd lng="hu">empire</trd>
  <trd lng="pt">imp&#233;rio</trd>
  <trd lng="ru">&c_a;&c_m;&c_p;&c_i;&c_r;</trd>
</drv>
</art>
