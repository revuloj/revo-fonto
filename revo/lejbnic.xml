<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: lejbnic.xml,v 1.10 2024/01/15 16:14:09 revo Exp $">
    <kap><rad>Lejbnic</rad>/o</kap>

    <drv mrk="lejbnic.0o">
      <kap><tld/>o<fnt><bib>PIV1</bib></fnt></kap>
      <snc mrk="lejbnic.0o.PERS">
        <uzo tip="fak">PERS</uzo>
        <dif>
          Granda germana filozofo kaj matematikisto
          (<trd lng="de">Gottfried Wilhelm von
          <ind>Leibniz</ind></trd>, 1646&ndash;1716):
          <ekz>
            Sed la regulo de <tld/>o montras ke <frm>(xv')' =
            xv''+v'</frm>
            <fnt>
              <aut>Christer Kiselman</aut>:
              <vrk><url ref="http://www2.math.uu.se/~kiselman/distribucioj.pdf">Enkonduko al
              distribucioj</url></vrk>.
              <lok>Acta Sanmarinensia IV.4</lok>
            </fnt>.        
          </ekz>
        </dif>
        <trdgrp lng="zh">
    <trd>&#x6208;&#x7279;&#x5F17;&#x88E1;&#x5FB7;&#x5A01;&#x5EC9;&#x99AE;&#x840A;&#x5E03;&#x5C3C;&#x8328; <pr>g&#x113;t&#xE8;f&#xFA;l&#x1D0;d&#xE9;w&#x113;ili&#xE1;nf&#xE9;ngl&#xE1;ib&#xF9;n&#xED;c&#xED;</pr></trd>,
    <trd>&#x6208;&#x7279;&#x5F17;&#x91CC;&#x5FB7;&#x5A01;&#x5EC9;&#x51AF;&#x83B1;&#x5E03;&#x5C3C;&#x8328; <pr>g&#x113;t&#xE8;f&#xFA;l&#x1D0;d&#xE9;w&#x113;ili&#xE1;nf&#xE9;ngl&#xE1;ib&#xF9;n&#xED;c&#xED;</pr></trd>,
    <trd>&#x6208;&#x7279;&#x5F17;&#x88E1;&#x5FB7;&#x5A01;&#x5EC9;&#x840A;&#x5E03;&#x5C3C;&#x8328; <pr>g&#x113;t&#xE8;f&#xFA;l&#x1D0;d&#xE9;w&#x113;ili&#xE1;nl&#xE1;ib&#xF9;n&#xED;c&#xED;</pr></trd>,
    <trd>&#x6208;&#x7279;&#x5F17;&#x91CC;&#x5FB7;&#x5A01;&#x5EC9;&#x83B1;&#x5E03;&#x5C3C;&#x8328; <pr>g&#x113;t&#xE8;f&#xFA;l&#x1D0;d&#xE9;w&#x113;ili&#xE1;nl&#xE1;ib&#xF9;n&#xED;c&#xED;</pr></trd>,
    <trd>&#x6208;&#x7279;&#x5F17;&#x88E1;&#x5FB7;&#x840A;&#x5E03;&#x5C3C;&#x8328; <pr>g&#x113;t&#xE8;f&#xFA;l&#x1D0;d&#xE9;l&#xE1;ib&#xF9;n&#xED;c&#xED;</pr></trd>,
    <trd>&#x6208;&#x7279;&#x5F17;&#x91CC;&#x5FB7;&#x83B1;&#x5E03;&#x5C3C;&#x8328; <pr>g&#x113;t&#xE8;f&#xFA;l&#x1D0;d&#xE9;l&#xE1;ib&#xF9;n&#xED;c&#xED;</pr></trd>,
    <trd>&#x83B1;&#x5E03;&#x5C3C;&#x8328; <pr>l&#xE1;ib&#xF9;n&#xED;c&#xED;</pr></trd>,
    <trd>&#x840A;&#x5E03;&#x5C3C;&#x8328; <pr>l&#xE1;ib&#xF9;n&#xED;c&#xED;</pr></trd>,
    <trd>&#x6208;&#x7279;&#x5F17;&#x88E1;&#x5FB7;&#x5A01;&#x5EC9; <pr>g&#x113;t&#xE8;f&#xFA;l&#x1D0;d&#xE9;w&#x113;ili&#xE1;n</pr></trd>,
    <trd>&#x6208;&#x7279;&#x5F17;&#x91CC;&#x5FB7;&#x5A01;&#x5EC9; <pr>g&#x113;t&#xE8;f&#xFA;l&#x1D0;d&#xE9;w&#x113;ili&#xE1;n</pr></trd>
  </trdgrp>
</snc>
      <trd lng="be">&c_L;&c_ja;&c_j;&c_b;&c_n;&c_ib;&c_c;</trd>
      <trd lng="ca">Leibniz</trd>
      <trd lng="fr">Leibniz</trd>
      <trd lng="ja">&#x30E9;&#x30A4;&#x30D7;&#x30CB;&#x30C3;&#x30C4;</trd>
  <trd lng="pl">Gottfried Wilhelm Leibniz</trd>     
      <trd lng="ro">Gottfried Wilhelm Leibniz</trd>
      <trd lng="ru">&c_L;&c_je;&c_j;&c_b;&c_n;&c_i;&c_c;</trd>
      <trd lng="uk">&#x41B;&c_je;&c_j;&c_b;&#x43D;&c_ib;&c_c;</trd>
</drv>

    <drv mrk="lejbnic.0a">
      <kap><tld lit="l"/>a</kap>
      <snc>
        <dif>
          Rilata a&ubreve; apartenanta al <tld/>o:
          <ekz>
            Louis Couturat <klr>...</klr> kuratoris gravan kritikan
            eldonon de <tld lit="l"/>aj verkoj kaj fari&gcirc;is
            dissendisto
            de la
            pensaro de la <tld lit="l"/>a penso ekde la komenco de la
            dudeka
            jarcento
            <fnt>
              <aut>Federico Gobbo</aut>:
              <vrk><url ref="http://www.dicom.uninsubria.it/~fgobbo/486-1986-5-PB.pdf">Fundamentoj
              de la adpoziciaj gramatikoj</url></vrk>
              <lok>Interlingvistikaj kajeroj, 1:1 (2010), ISSN
              2037-4550</lok>
            </fnt>.
          </ekz>
         </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_l;&c_ja;&c_j;&c_b;&c_n;&c_ib;&c_c;&c_a;&c_w;</trd>,
        <trd>&c_L;&c_ja;&c_j;&c_b;&c_n;&c_ib;&c_c;&c_a;</trd>
      </trdgrp>
      <trd lng="ca">leibnizi&agrave;</trd>
      <trd lng="fr">leibnizien</trd>
      <trd lng="pl">Leibniza</trd>
      <trdgrp lng="ru">
        <trd>&c_l;&c_je;&c_j;&c_b;&c_n;&c_i;&c_c;&c_je;&c_v;</trd>,
        <trd>&c_l;&c_je;&c_j;&c_b;&c_n;&c_i;&c_c;&c_je;&c_v;&c_s;&c_k;&c_i;&c_j;</trd>
      </trdgrp>
    </drv>

  </art>

