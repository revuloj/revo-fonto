<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vojevodi.xml,v 1.15 2023/04/18 05:27:34 revo Exp $">
<kap>
  <rad>vojevodi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="vojevodi.0o">
  <kap><tld/>o</kap>
  <snc mrk="vojevodi.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="gubern1.0o.POL">Gubernio</ref>, historie en
      pluraj oriente&ubreve;ropaj landoj, nuntempe en Pollando:
      <ekz>
        <nom>Grudzi&aogonek;dz</nom> efektive rolis kiel la &ccirc;efurbo de pomeria <tld/>o,
        grava kultura kaj kleriga centro
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.polskieradio.pl/eo/dokument.aspx?iid=51390">
          Pollando invitas: Grudzi&aogonek;dz</url></vrk>,
          <lok>Pola Radio, 2007-04-21</lok>
        </fnt>.
      </ekz>
      <ekz>
        plej volonte la anglan lingvon lernas lernantoj de la silezia <tld/>o kaj la
        germanan en lernejoj de la <tld/>oj situantaj la&ubreve;longe de la okcidenta limo
        de Pollando
        <fnt>
          <aut>Stanis&lstroke;aw &Sacute;migielski</aut>
          <vrk><url ref="&Monato;/2000/007193.html">Instruisto de la angla ser&ccirc;ata</url></vrk>
          <bib>Monato</bib>
          <lok>2001/02, p. 19</lok>
          </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="vojevod.0ujo">vojevodujo</ref>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_ja;&c_v;&c_o;&c_d;&c_a;</trd>
  <trd lng="ca">voivodat</trd>
  <trd lng="cs">vojvodstv&iacute;</trd>
  <trd lng="de">Woiwodschaft</trd>
  <trd lng="en">voivodship</trd>
  <trd lng="fr">vo&iuml;vodie</trd>
  <trd lng="hu">vajdas&aacute;g</trd>
  <trd lng="nl">woiwodschap <klr tip="ind">(Poolse
  provincie)</klr></trd>
  <trd lng="pl">wojew&oacute;dztwo</trd>
  <trd lng="sk">vojvodstvo</trd>
  <trd lng="uk">&c_v;&#x43E;&c_jeu;&c_v;&#x43E;&c_d;&c_s;&c_t;&c_v;&#x43E;</trd>
</drv>

<drv mrk="vojevodi.0estro">
  <kap><tld/>estro</kap>
  <snc mrk="vojevodi.0estro.vojevodo">
    <dif>
      <ref tip="dif" cel="vojevod.0o">Vojevodo</ref>:
      <ekz>
        melo <tld/>estro, /
        &ccirc;e vulpo estis servisto
        <fnt>
        <aut>J. Brzechwa, trad. D. Kowalska</aut>,
        <vrk><url ref="http://esperanto.pl/page.php?tid=311259">
        Friponecoj de Vulpo Vitaliso</url></vrk>,
        <lok>[vidita en 2007-07]</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="duk.0o">duko</ref>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_ja;&c_v;&c_o;&c_d;&c_s;&c_t;&c_v;&c_a;</trd>
  <trd lng="ca">voivoda</trd>
  <trd lng="de">Woiwode</trd>
  <trd lng="en">voivode</trd>
  <trd lng="fr">vo&iuml;vode</trd>
  <trd lng="hu">vajda</trd>
  <trd lng="pl">wojewoda</trd>
  <trd lng="nl">woiwode</trd>
</drv>
</art>

