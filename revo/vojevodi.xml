<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>vojevodi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="vojevodi.0o">
  <kap><tld/>o</kap>
  <snc mrk="vojevodi.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="gubern1.0o.POL">Gubernio</ref>, historie en
      pluraj oriente&ubreve;ropaj landoj, nuntempe en Pollando:
      <ekz>
        Grudzi&aogonek;dz efektive rolis kiel la &ccirc;efurbo de pomeria <tld/>o,
        grava kultura kaj kleriga centro
        <fnt>
        <aut>-</aut>,
        <vrk><url
        ref="http://www.polskieradio.pl/eo/dokument.aspx?iid=51390">
        Pollando invitas: Grudzi&aogonek;dz</url></vrk>,
        <lok>Pola Radio, 2007-04-21</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">vo&iuml;vodie</trd>
  <trd lng="hu">vajdas&aacute;g</trd>
  <trd lng="nl">woiwodschap <klr tip="ind">(Poolse
  provincie)</klr></trd>
</drv>

<drv mrk="vojevodi.0estro">
  <kap><tld/>estro</kap>
  <ref tip="dif" cel="vojevod.0o">vojevodo</ref>
  <ekz>
    Melo <tld/>estro, /
    &Ccirc;e vulpo estis servisto
    <fnt>
    <aut>J. Brzechwa, trad. D. Kowalska</aut>,
    <vrk><url ref="http://esperanto.pl/page.php?tid=311259">
    Friponecoj de Vulpo Vitaliso</url></vrk>,
    <lok>[vidita en 2007-07]</lok>
    </fnt>.
  </ekz>
  <trd lng="fr">vo&iuml;vode</trd>
  <trd lng="hu">vajda</trd>
  <trd lng="nl">woiwode</trd>
</drv>
</art>

