<art mrk="$Id: azar.xml,v 1.23 2023/08/30 18:23:38 revo Exp $">
<kap>
  <rad>azar</rad>/o
</kap>
<drv mrk="azar.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio
      <ref tip="malprt" cel="aristo3.0oj">aristolokiacoj</ref>
      (<trd lng="la">Asarum</trd>) kun kutime renformaj folioj:
      <ekz>
        en la 19-a jarcento la rizomo de diversaj specioj de la <tld lit="A"/>o estis
        prilaboritaj kiel <klr>[&#x2026;]</klr> laksigilo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=257953">Azaro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/7/74/Asarum_kz.JPG/640px-Asarum_kz.JPG" prm="CC-BY-SA-3.0">folioj de E&ubreve;ropa <tld/>o (Asarum europaeum)
        <fnt>
          <aut>Krzysztof Ziarnek, propra verko</aut>
          <lok><url ref="&WCW;/File:Asarum_kz.JPG">&WCW;/File:Asarum_kz.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_a;&c_l;&c_je;&c_sh;&c_n;&c_ib;&c_k;</trd>
  <trd lng="cs">kopytn&iacute;k</trd>
  <trd lng="de">Haselwurz</trd>
  <trd lng="en">ginger</trd>
  <trd lng="es">&aacute;saro</trd>
  <trd lng="hu">kapotnyak</trd>
  <trdgrp lng="it">
    <trd>asaro</trd>,
    <trd>ginger selvatico</trd>
  </trdgrp>
  <trd lng="ja">&#x30D5;&#x30BF;&#x30D0;&#x30A2;&#x30AA;&#x30A4;</trd>
  <trd lng="nl">mansoor</trd>
  <trd lng="pt">&aacute;saro</trd>
  <trd lng="ru">&c_k;&c_o;&c_p;&c_y;&c_t;&c_je;&c_n;&c_mol;</trd>
  <trd lng="sk">kopytn&iacute;k</trd>
  <trd lng="sv">hassel&ouml;rt</trd>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x43F;&#x438;&#x442;&#x43D;&#x44F;&#x43A;</trd>,
    <trd>&#x43A;&#x43E;&#x43F;&#x438;&#x442;&#x43D;&#x438;&#x43A;</trd>
  </trdgrp>
</drv>
</art>
