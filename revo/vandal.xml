<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vandal.xml,v 1.28 2024/04/12 17:25:52 revo Exp $">
<kap>
  <rad>vandal</rad>/o
  <fnt><bib>FK</bib></fnt>
</kap>

<drv mrk="vandal.0o">
  <kap><tld/>o</kap>
  <snc mrk="vandal.0o.HIS">
  <uzo tip="fak">HIS</uzo>
    <dif>
      Ano de <ref tip="super" cel="gxerman.0o">&gcirc;ermana</ref> popolo,
      kiu
      forlasis Silezion en la 3a jarcento, trairis E&ubreve;ropon, Hispanion,
      perbatalis nordokcidentan Afrikon kaj mediteraneajn insulojn, kaj
      regis tiujn teritoriojn de 429 &gcirc;is la Romia rekonkero en 534.
    </dif>
    <trd lng="pt">v&acirc;ndalo<klr tip="amb"> (hist&oacute;ria)</klr></trd>
  </snc>
  <snc mrk="vandal.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Difektanto, detruanto de monumentoj, arta&jcirc;oj a&ubreve; atingeblaj
      poseda&jcirc;oj:
      <ekz>
        tion li nomis &leftquot;vandalaj turmentoj&rightquot;, volante diri &leftquot;Tantalaj turmentoj&rightquot;
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        beda&ubreve;rinde la domo dum longa tempo restadis vaka, kaj &gcirc;i estis terure difektita
        de <tld/>oj kaj de la malseka mez-irlanda vetero
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2008/009326.php">Analoj de Bailenambeann</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        la homoj, kiuj komence simpatiis kun ili, balda&ubreve; trovis, ke ne
        la <tld/>oj, sed la financistoj pravas
        <fnt><aut>Ji&rcaron;i Patera</aut>:
          <vrk>Post la kunsido de financistoj en Prago</vrk>,
          <bib>Monato</bib><lok>jaro 2001a, numero 1a, p. 12a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="pt">v&acirc;ndalo</trd>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_n;&c_d;&c_a;&c_l;</trd>
  <trd lng="bg">&c_v;&c_a;&c_n;&c_d;&c_a;&c_l;</trd>
  <trd lng="ca">v&agrave;ndal</trd>
  <trdgrp lng="cs">
    <trd>kazisv&ecaron;t</trd>,
    <trd>ni&ccaron;itel</trd>,
    <trd>vandal</trd>
  </trdgrp>
  <trd lng="de">Vandale</trd>
  <trd lng="el">&beta;&alfa_ton;&ny;&delta;&alfa;&lambda;&omikron;&sigma_fina;</trd>
  <trd lng="en">vandal</trd>
  <trd lng="es">v&aacute;ndalo</trd>
  <trd lng="fr">vandale</trd>
  <trd lng="hu">vand&aacute;l</trd>
  <trdgrp lng="ja">
    <trd>&#x30D0;&#x30F3;&#x30C0;&#x30EB;&#x65CF; 
    <pr>&#x30D0;&#x30F3;&#x30C0;&#x30EB;&#x305E;&#x304F;</pr></trd>,
    <trd>&#25991;&#21270;&#x8CA1;&#x306E;&#30772;&#x58CA;&#32773;  <pr>&#12406;&#12435;&#x304B;&#12374;&#12356;&#x306E;&#x306F;&#x304B;&#12356;&#12375;&#12419;</pr></trd>
  </trdgrp>
  <trd lng="nl">vandaal</trd>
  <trdgrp lng="pl">
    <trd>wandal</trd>,
    <trd>barbarzy&nacute;ca</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_a;&c_n;&c_d;&c_a;&c_l;</trd>
  <trdgrp lng="sk">
    <trd>kazisvet</trd>,
    <trd>ni&ccaron;ite&lcaron;</trd>,
    <trd>vandal</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_a;&#x43D;&c_d;&c_a;&#x43B;</trd>,
    <trd>&c_v;&c_a;&c_r;&c_v;&c_a;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="vandal.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      Senkiala detruado de monumentoj, publikaj a&ubreve; publike atingeblaj
      hava&jcirc;oj:
      <ekz>
        ne maloftis <tld/>ismo kaj oportunisma &scirc;telado
        <fnt>
          <bib>Monato</bib>
          <aut>Jozefo Lej&ccirc;</aut>
          <vrk><url ref="&Monato;/2013/011087.php">Veki&gcirc;as giganto</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio jam timigas la publikon, &ccirc;ar okazas detruado de tombejoj,
        vandalismo, pri&scirc;telado<fnt><aut>Saliko</aut>: <vrk>Je kio ni
        kredas?</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 11a, p.
        12a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_n;&c_d;&c_a;&c_l;&c_ib;&c_z;&c_m;</trd>
  <trd lng="bg">&c_v;&c_a;&c_n;&c_d;&c_a;&c_l;&c_i;&c_z;&c_malmol;&c_m;</trd>
  <trd lng="ca">vandalisme</trd>
  <trd lng="cs">vandalizmus</trd>
  <trd lng="de">Vandalismus</trd>
  <trd lng="en">vandalism</trd>
  <trdgrp lng="fr">
    <trd>vandalisme</trd>
  </trdgrp>
  <trd lng="hu">vandalizmus</trd>
  <trdgrp lng="ja">
    <trd>&#30772;&#x58CA;&#x884C;&#x70BA;  <pr>&#x306F;&#x304B;&#12356;&#12371;&#12358;&#12356;</pr></trd>,
    <trd>&#x86EE;&#x884C;  <pr>&#12400;&#12435;&#12371;&#12358;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wandalizm</trd>,
    <trd>barbarzy&nacute;stwo</trd>
  </trdgrp>
  <trd lng="pt">vandalismo</trd>
  <trd lng="sk">vandalizmus</trd>
  <trd lng="uk">&c_v;&c_a;&#x43D;&c_d;&c_a;&#x43B;&c_ib;&c_z;&#x43C;</trd>
</drv>
</art>
