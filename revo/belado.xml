<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>beladon</rad>/o
</kap>
<drv mrk="belado.0o">
<kap><ofc>*</ofc><tld/>o</kap>
<uzo tip="fak">BOT</uzo>
<snc>
<dif>
Specio el familio <ref tip="super" cel="solana.0oj">solanacoj</ref>
(<trd lng="la">Atropa belladona</trd>), venena arbedo, el kiu oni ekstraktas
<ref cel="atropi.0o">atropinon</ref>.
</dif>
</snc>
<trd lng="de">Tollkirsche</trd>
<trdgrp lng="en">
   <trd>deadly <ind>nightshade</ind></trd>,
   <trd>devil's cherry</trd>,
   <trd>belladonna</trd>
</trdgrp>
<trd lng="fr">belladone</trd>
<trd lng="hu">(maszlagos) <ind>nadragulya</ind></trd>
<trdgrp lng="ru">
   <trd>&c_b;&c_je;&c_l;&c_a;&c_d;&c_o;&c_n;&c_n;&c_a;</trd>,
   <trd>&c_k;&c_r;&c_a;&c_s;&c_a;&c_v;&c_k;&c_a;</trd>
</trdgrp>
<trd lng="sv">belladonna</trd>
</drv>
</art>
