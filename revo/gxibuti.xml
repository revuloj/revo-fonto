<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&gcirc;ibuti</rad>/o
</kap>

<drv mrk="gxibuti.0o">
  <kap>
    <tld lit="&Gcirc;"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="gxibuti.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la nordorienta <ref tip="malprt"
      cel="afrik.A0o">Afriko</ref>
      kun &ccirc;efurbo &Gcirc;ibutio.
      Teritorio de Respubliko &Gcirc;ibutio.
      ISO-3166 kodo: DJ.
      &Gcirc;i limas kun
      <ref tip="vid" cel="eritre.0o.GEOG">Eritreo</ref>,
      <ref tip="vid" cel="etiop.0ujo.GEOG">Etiopujo</ref>,
      <ref tip="vid" cel="somal.0ujo.GEOG">Somalujo</ref>,
      kaj markolo Bab al-Mandab.
    </dif>
    <trdgrp lng="ar">
      <trd>&jim;&ya;&ba;&waw;&ta;&ya;</trd>,
      <trd>&jim;&mim;&ha;&waw;&ra;&ya;&ta_marbuta; &jim;&ya;&ba;&waw;&ta;&ya;</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>D&zdot;ibuti</trd>,
      <trd>Republika <ind>D&zdot;ibuti</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

