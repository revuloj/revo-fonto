<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gxibuti.xml,v 1.17 2023/11/11 11:36:38 revo Exp $">
<kap>
  <rad>&Gcirc;ibuti</rad>/o
  <fnt><bib>RLN</bib></fnt>
</kap>

<drv mrk="gxibuti.GX0o">
  <kap>
    <tld lit="&Gcirc;"/>o
  </kap>
  <mlg kod="ISO-3166">DJ</mlg>
  <snc mrk="gxibuti.GX0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o.POL">&Scirc;tato</ref>
      en la nordorienta <ref tip="malprt" cel="afrik.A0o">Afriko</ref>
      kun &ccirc;efurbo <ref tip="prt" cel="gxibut.0o">&Gcirc;ibuto</ref>; teritorio de Respubliko 
      <tld/>o, limanta kun
      <ref tip="vid" cel="eritre.E0o.GEOG">Eritreo</ref>,
      <ref tip="vid" cel="etiop.E0ujo.GEOG">Etiopujo</ref>,
      <ref tip="vid" cel="somal.S0ujo.GEOG">Somalujo</ref>,
      kaj markolo <nom>Bab al-Mandab</nom>:
      <ekz>
        Francio estas la &gcirc;endarmo de la franclingva kaj malproksima Afriko, &gcirc;iaj
        soldatoj havas kazernojn en Senegalo, Ebur-Bordo, Gabono, &Ccirc;ado, Centr-Afriko
        kaj <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Yves Nicolas</aut>
          <vrk><url ref="&Monato;/2013/010985.php">Malio kaj la granda mutulo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> &gcirc;enerala sekretario de la sindikato de instruistoj SEP, estis
        arestita <klr>[en]</klr> marto 2017 fare de la sekurservoj de <tld/>o
        <fnt>
          <vrk><url ref="https://www.labourstartcampaigns.net/show_campaign.cgi?c=3379">&Gcirc;ibutio:
          liberigu la gvidantojn de la sindikato de la ...</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_D;&c_zh;&c_y;&c_b;&c_u;&c_t;&c_y; <klr tip="amb">(&c_k;&c_r;&c_a;&c_ib;&c_n;&c_a;)</klr></trd>
    <trdgrp lng="ar">
      <trd>&a_j;&a_y;&a_b;&a_w;&a_t;&a_y;</trd>,
      <trd>&a_j;&a_m;&a_h;&a_w;&a_r;&a_y;&a_t_marbuta; &a_j;&a_y;&a_b;&a_w;&a_t;&a_y;</trd>
    </trdgrp>
    <trd lng="bg">&c_D;&c_zh;&c_i;&c_b;&c_u;&c_t;&c_i;</trd>
    <trdgrp lng="ca">
      <trd>Djibouti</trd>,
      <trd>Rep&uacute;blica de Djibouti</trd>
    </trdgrp>
    <trd lng="de">Dschibuti</trd>
    <trd lng="en">Djibouti</trd>
    <trd lng="fa">&a_j;&f_y;&a_b;&a_w;&a_t;&f_y;</trd>
    <trd lng="fr">Djibouti</trd>
    <trd lng="id">Djibouti</trd>
    <trd lng="nl">Djibouti</trd>
    <trdgrp lng="pl">
      <trd>D&zdot;ibuti</trd>,
      <trd>Republika <ind>D&zdot;ibuti</ind></trd>
    </trdgrp>
    <trd lng="pt">Djibouti</trd>
    <trd lng="zh">&#x5409;&#x5E03;&#x63D0; <pr>j&#xED;b&#xF9;t&#xED;</pr></trd>
</snc>
  <trd lng="ja">&#x30B8;&#x30D6;&#x30C1;</trd>
  <trd lng="uk">&c_D;&c_zh;&c_ib;&c_b;&c_u;&c_t;&c_ib;</trd>
</drv>

</art>

