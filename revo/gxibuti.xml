<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&gcirc;ibuti</rad>/o
</kap>

<drv mrk="gxibuti.0o">
  <kap>
    <tld lit="&Gcirc;"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="gxibuti.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la nordorienta <ref tip="malprt"
      cel="afrik.A0o">Afriko</ref>
      kun &ccirc;efurbo &Gcirc;ibutio.
      Teritorio de Respubliko &Gcirc;ibutio.
      ISO-3166 kodo: DJ.
      &Gcirc;i limas kun
      <ref tip="vid" cel="eritre.0o.GEOG">Eritreo</ref>,
      <ref tip="vid" cel="etiop.0ujo.GEOG">Etiopujo</ref>,
      <ref tip="vid" cel="somal.0ujo.GEOG">Somalujo</ref>,
      kaj markolo Bab al-Mandab.
    </dif>
    <trdgrp lng="ar">
      <trd>&a_j;&a_y;&a_b;&a_w;&a_t;&a_y;</trd>,
      <trd>&a_j;&a_m;&a_h;&a_w;&a_r;&a_y;&a_t_marbuta; &a_j;&a_y;&a_b;&a_w;&a_t;&a_y;</trd>
    </trdgrp>
    <trd lng="bg">&c_D;&c_zh;&c_i;&c_b;&c_u;&c_t;&c_i;</trd>
    <trd lng="de">Dschibuti</trd>
    <trd lng="en">Djibouti</trd>
    <trd lng="fa">&a_j;&f_y;&a_b;&a_w;&a_t;&f_y;</trd>
    <trdgrp lng="pl">
      <trd>D&zdot;ibuti</trd>,
      <trd>Republika <ind>D&zdot;ibuti</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

