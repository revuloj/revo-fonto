<drv mrk="rost.krad0i">
  <kap>krad<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="kuir.0i">Kuiri</ref> man&gcirc;a&jcirc;on
      sur krado super fajro:
      <ekz>
        fre&scirc;a fi&scirc;a&jcirc;o krad<tld/>ita kun ajlo
        <fnt>
          <bib>MD17</bib>
          <vrk>Kia socialismo post la fratoj Castro?</vrk>
        </fnt>;
      </ekz>
      <ekz>
        pekla&jcirc;oj kaj <ind>krad<tld/>a sa&ubreve;co</ind>
        <fnt>
          <bib>MD14</bib>
          <vrk>Malanta&ubreve; la etika kafo kaj la saniga sandvi&ccirc;o</vrk>
        </fnt>;
        <trd lng="de">Grillso&szlig;e</trd>
      </ekz>
      <ekz>
        oni devis preteriri <ind>krad<tld/>budo</ind>n kaj etan infanludejon
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2004/008334.html">Centra demando</url></vrk>
          <lok>2004</lok>
        </fnt>;
        <trd lng="de">Grillstand</trd>
      </ekz>
      <ekz>
        &ccirc;iuj &ndash; je la fino de la ekskurso &ndash; honoras la <ind>krad<tld/>a&jcirc;o</ind>n
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2011/010394.php">&Ccirc;iam pli malalten</url></vrk>
          <lok>2011</lok>
        </fnt>;
        <trd lng="de">Grillgut</trd>
      </ekz>
      <ekz>
        krad<tld/>bakado de fi&scirc;oj, kolbasetoj kaj bananoj
        <fnt>
          <bib>LOdE</bib>
          <vrk>HELA en Hel kaj sur Hel</vrk>
        </fnt>;
      </ekz>
      <ekz>
        tabloj, benkoj, <ind>krad<tld/>ejo</ind> kaj sporta tereno, &gcirc;i estas trankvila loko
        <fnt>
          <bib>LOdE</bib>
          <vrk>Infanoj atendataj en la nordo</vrk>
        </fnt>.
        <trd lng="de">Grillstand</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_m;&c_a;&c_zh;&c_y;&c_c;&c_mol; &c_n;&c_a;
<ind>&c_g;&c_r;&c_y;&c_l;&c_ib;</ind></trd>
  <trd lng="bo">&t_ma;&t_drengbu;&t_tsheg;&t_ba;&t_ka;&t_rata;&t_ga;&t_tsheg;</trd>
  <trd lng="cs">op&eacute;kat na ro&scaron;tu</trd>
  <trd lng="en">grill</trd>
  <trd lng="fr">faire <ind>griller</ind></trd>
