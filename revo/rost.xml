<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rost.xml,v 1.45 2023/09/03 07:53:12 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>rost</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="rost.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rost.0i.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="kuir.0i">Kuiri</ref> man&gcirc;a&jcirc;on elmetante &gcirc;in
      al varmego sur kuirkrado, sur <tld/>ostango, en forno, en elektra aparato a&ubreve; en
      kaserolo, ne uzante grason nek akvon:
      <ekz>
        <tld/>i &scirc;afon super la fajro;
      </ekz>
      <ekz>
        <tld/>i terpomojn, grajnojn, panon, kafon, tabakon;
      </ekz>
      <ekz>
        ili donis al li pecon de rostita fi&scirc;o
        <fnt>
          <bib>NT</bib>
          <lok>Luko 24:42</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne man&gcirc;u <klr>[la &scirc;afidon]</klr> duonkrudan, nek kuiritan en akvo,
        sed nur <tld/>itan sur fajro kun &gcirc;iaj kapo, kruroj, kaj interna&jcirc;oj
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 12:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        panon kaj <tld/>itajn grajnojn kaj fre&scirc;ajn grajnojn ne man&gcirc;u &gcirc;is
        tiu tago
        mem, en kiu vi alportos la oferon al via Dio
        <fnt><bib>MT</bib>, <lok>Levidoj 23:14</lok></fnt>;
      </ekz>
      <ekz>
        ekstere la virinoj <tld/>is la Paskan &scirc;afidon
        <fnt><bib>Fab1</bib>, <lok>Ligo de amikeco</lok></fnt>;
      </ekz>
      <ekz>
        mia patrino faris fajron, <tld/>is la bulbojn, kiujn &scirc;i kunportis kun si
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ita ansero far&ccirc;ita per prunoj kaj pomoj
        <fnt><bib>Fab2</bib>, <lok>Knabineto kun alumetoj</lok></fnt>;
      </ekz>
      <ekz>
        oni <tld/>is tutajn bovojn
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 21</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>ita kolombeto ne flugas al bu&scirc;eto
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni devis preteriri krad<tld/>budon kaj etan infanludejon
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki LINS</aut>
          <vrk><url ref="&Monato;/2004/008334.html">Centra demando</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bak.0i">baki</ref>,
      <ref cel="frit.0i">friti</ref>
    </refgrp>
    <trd lng="hu">s&uuml;t</trd>
  </snc>
  <snc mrk="rost.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Eligi varmegon, <ref tip="super" cel="ard.0i">ardi</ref>:
      <ekz>
        kiel ajn varmege &ccirc;i tie <tld/>as la suno, unu pa&scirc;on plue estas
        glacie malvarme
        <fnt><bib>Fab1</bib>, <lok>&Gcirc;ardeno de la paradizo</lok></fnt>;
      </ekz>
      <ekz>
        sur la &ccirc;ielo senhalte brulis la suno, kies &ccirc;iu radio <tld/>is
        kaj blindigis
        <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 19</lok></fnt>.
      </ekz>
    </dif>
  <trdgrp lng="hu">
    <trd>&eacute;get <klr tip="amb">(&aacute;tv.)</klr></trd>,
    <trd>perzsel <klr tip="amb">(&aacute;tv.)</klr></trd>
  </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_m;&c_a;&c_zh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_zh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_ja;&c_ch;&c_y;</trd>
  </trdgrp>
  <trd lng="bo">&t_ra;&t_ngata;&t_naro;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>op&eacute;kat</trd>,
    <trd>p&eacute;ci (maso)</trd>,
    <trd>pra&zcaron;it</trd>,
    <trd>p&eacute;ci <klr>(maso)</klr></trd>,
    <trd>sma&zcaron;it</trd>,
    <trd>upra&zcaron;it</trd>,
    <trd>&scaron;kva&rcaron;it</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>grillen</trd>,
    <trd>r&ouml;sten</trd>
  </trdgrp>
  <trd lng="en">roast</trd>
  <trd lng="fr">faire <ind>r&ocirc;tir</ind></trd>
  <trdgrp lng="he">
    <trd>&lamed;&tsadi;&lamed;&vav;&tav; \<ind>&tsadi;&lamed;&he;</ind>\</trd>,
    <trd>&lamed;&qof;&lamed;&vav;&tav; \<ind>&qof;&lamed;&he;</ind>\</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x713C;&#x304F;  [<ind>&#x3084;&#x304F;</ind>]</trd>,
    <trd>&#x3042;&#x3076;&#x308B;</trd>,
    <trd>&#x30ED;&#x30FC;&#x30B9;&#x30C8;&#x3059;&#x308B;</trd>,
    <trd>&#x714E;&#x308B;  [<ind>&#x3044;&#x308B;</ind>]</trd>,
    <trd>&#x5F37;&#x3044;&#x71B1;&#x306B;&#x3055;&#x3089;&#x3059;  [<ind>&#x3064;&#x3088;&#x3044;&#x306D;&#x3064;&#x306B;&#x3055;&#x3089;&#x3059;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">roosteren</trd>
  <trdgrp lng="pl">
    <trd>sma&zdot;y&cacute;</trd>,
    <trd>grillowa&cacute;</trd>,
    <trd>piec<klr>  (kulin.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>assar</trd>,
    <trd>torrar</trd>,
    <trd>torrificar</trd>,
    <trd>tostar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_a;&c_r;&c_i;&c_t;&c_mol; <klr tip="amb">(&c_v;
    &c_d;&c_u;&c_h;&c_o;&c_v;&c_k;&c_je;, &c_n;&c_a; &c_o;&c_g;&c_n;&c_je;,
    &c_b;&c_je;&c_z; &c_m;&c_a;&c_s;&c_l;&c_a;)</klr></trd>,
    <trd>&c_p;&c_je;&c_ch;&c_mol; <klr tip="amb">(&c_g;&c_l;&c_a;&c_g;&c_o;&c_l;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>opeka&tcaron;</trd>,
    <trd>piec&tcaron;</trd>,
    <trd>pra&zcaron;i&tcaron;</trd>,
    <trd>sma&zcaron;i&tcaron;</trd>,
    <trd>&scaron;kvari&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_zh;&c_a;&c_r;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_m;&c_a;&c_zh;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_s;&c_m;&c_a;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_je;&c_k;&c_t;&c_i; &c_n;&c_a; &c_v;&c_o;&c_g;&c_n;&c_ib;</trd>
  </trdgrp>
</drv>

<drv mrk="rost.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="rost.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="R"/>ita man&gcirc;a&jcirc;o:
      <ekz>
        mi diris al la kuiristo ke la birdoj estas donaco,
        li do faris el ili <tld/>a&jcirc;ojn kaj paste&ccirc;ojn
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 18</lok></fnt>;
      </ekz>
      <ekz>
        ni havas bonegan kapreolan <tld/>a&jcirc;on
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 19</lok></fnt>;
      </ekz>
      <ekz>
        la odoro de <tld/>a&jcirc;o plenigis &ccirc;iujn &ccirc;ambrojn
        <fnt><bib>Fab4</bib>, <lok>Filino de la mar&ccirc;a re&gcirc;o</lok></fnt>;
      </ekz>
      <ekz>
        teo kun unu pan<tld/>a&jcirc;o
        <klr>(vd <ref cel="rost.0pano">rostpano</ref>)</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2009/009860.php">Voja&gcirc;romano klasika</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj <klr>[&#x2026;]</klr> honoras la krad<tld/>a&jcirc;on, <klr>[&#x2026;]</klr> kiun, dum la tuta mateno,
        kelke da volontuloj kun enga&gcirc;i&gcirc;o pretigis
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2011/010394.php">&Ccirc;iam pli malalten</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_m;&c_a;&c_zh;&c_a;&c_n;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_s;&c_m;&c_a;&c_zh;&c_o;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="fr">r&ocirc;ti<klr> (subst.)</klr></trd>
  <trd lng="he">&tsadi;&lamed;&yod;</trd>
  <trd lng="hu">s&uuml;lt <klr tip="amb">(pl. h&uacute;s)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x713C;&#x304D;&#x3082;&#x306E;  [<ind>&#x3084;&#x304D;&#x3082;&#x306E;</ind>]</trd>,
    <trd>&#x713C;&#x8089;  [<ind>&#x3084;&#x304D;&#x306B;&#x304F;</ind>]</trd>,
    <trd>&#x713C;&#x304D;&#x9B5A;  [<ind>&#x3084;&#x304D;&#x3056;&#x304B;&#x306A;</ind>]</trd>
  </trdgrp>
  <trd lng="pl">piecze&nacute;</trd>
  <trd lng="ru">&c_zh;&c_a;&c_r;&c_k;&c_o;&c_je;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_m;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_ch;&c_je;&c_n;&c_ja;</trd>,
    <trd>&c_t;&c_o;&c_s;&c_t;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_s;&c_m;&c_a;&c_zh;&c_je;&c_n;&c_a; &c_s;&c_k;&c_i;&c_b;&c_k;&c_a; &c_h;&c_l;&c_ib;&c_b;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="rost.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="rost.0igxi.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Kuiri&gcirc;i per elmeti&gcirc;o al varmego:
      <ekz>
        la poeto sidis varme kaj komforte &ccirc;e sia kahela forno, en kiu brulis fajro kaj <tld/>i&gcirc;is
        pomoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Malbonkonduta knabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        sep grandaj viandopecoj <tld/>i&gcirc;is en la fajro
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_m;&c_a;&c_zh;&c_y;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_zh;&c_y;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_ja;&c_ch;&c_y;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="fr">r&ocirc;tir<klr> (intr.)</klr></trd>
  <trd lng="pl">piec si&eogonek;</trd>
  <trdgrp lng="uk">
    <trd>&c_zh;&c_a;&c_r;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_m;&c_a;&c_zh;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_je;&c_k;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="rost.0ilo">
  <kap>
    <tld/>ilo
  </kap>
  <snc>
    <dif>
      Aparato por <tld/>i:
      <ekz>
        la virino preparis la birdon, metis &gcirc;in sur <ind>turn<tld/>ilo</ind>n kaj komencis &gcirc;in
        <tld/>i
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
        <trd lng="de">Drehspie&szlig;</trd>
      </ekz>
      <ekz>
        oni portis metalajn vazojn kuirejajn: kradojn por la fornoj, turn<tld/>ilojn,
        dutenilajn potojn kaj platajn patojn kun kovriloj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        por rosti viandopecojn oni trapikas ilin per <tld/>ostango a&ubreve; metas sur
        <ind>krad<tld/>ilon</ind>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=305682">&Gcirc;ardenrostilo</url></lok>
        </fnt>;
        <trd lng="de">Grillrost</trd>
      </ekz>
      <ekz>
        mar&scirc;maloj estas tre &scirc;atataj en Usono <klr>[&#x2026;]</klr> la dol&ccirc;a&jcirc;oj estas <tld/>itaj sur
        krad<tld/>ilo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=603833">Mar&scirc;malo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        platajn pecojn de distran&ccirc;ita pano oni kutime rostas en <ind>pan<tld/>ilo</ind>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=286902">Rostpano</url></lok>
        </fnt>;
        <trd lng="be">&c_t;&c_o;&c_s;&c_t;&c_e;&c_r;</trd>
        <trd lng="cs">topinkova&ccaron;</trd>
        <trd lng="de">Toaster</trd>
        <trd lng="en">toaster</trd>
        <trd lng="es">tostador</trd>
        <trd lng="fr">grille-pain</trd>
        <trdgrp lng="he">
          <trd>&mem;&tsadi;&nun;&fmem;</trd>,
          <trd>&tet;&vav;&samekh;&tet;&resh;</trd>
        </trdgrp>
        <trd lng="hu">keny&eacute;rpir&iacute;t&oacute;</trd>
        <trd lng="it">tostapane</trd>
        <trd lng="nl">broodrooster</trd>
        <trd lng="pl">toster</trd>
        <trd lng="pt">torradeira</trd>
        <trd lng="ru">&c_t;&c_o;&c_s;&c_t;&c_je;&c_r;</trd>
        <trd lng="sk">hriankova&ccaron;</trd>
      </ekz>
      <ekz>
        La&ubreve;renco de Romo <klr>[&#x2026;]</klr> en la arta prezento <klr>[&#x2026;]</klr>
        bildi&gcirc;as kune kun la krad<tld/>ilo, sur kiu li estis torturita &gcirc;is morto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=275901">La&ubreve;renco de Romo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Grill</trd>,
    <trd>Rost</trd>,
    <trd>Spie&szlig;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x713C;&#x304D;&#x5668;  [<ind>&#x3084;&#x304D;&#x3046;&#x3064;&#x308F;</ind>]</trd>,
    <trd>&#x713C;&#x8089;&#x5668;  [<ind>&#x3084;&#x304D;&#x306B;&#x304F;&#x3046;&#x3064;&#x308F;</ind>]</trd>,
    <trd>&#x713C;&#x304D;&#x7DB2;  [<ind>&#x3084;&#x304D;&#x3042;&#x307F;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>piekarnik</trd>,
    <trd>grill</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_l;&c_i;&c_s;&c_t;</trd>,
    <trd>&c_s;&c_k;&c_o;&c_v;&c_o;&c_r;&c_o;&c_d;&c_a;</trd>,
    <trd>&c_zh;&c_a;&c_r;&c_o;&c_v;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="rost.sub0i">
  <kap>sub<tld/>i, <var><kap><tld/>eti</kap></var></kap>
  <snc mrk="rost.sub0i.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Iom <tld/>i, &gcirc;is &gcirc;i havas flavan a&ubreve; flave-brunan, sed ne plenbrunan, koloron:
      <ekz>
        en oleita pato ni anta&ubreve;kuire sub<tld/>as la hakitan cepon
        <fnt>
          <vrk><url ref="https://www.sat-amikaro.org/IMG/pdf/LS65-retuto.pdf">Seta Tjelo</url></vrk>
          <lok>La Sago, n-ro 65, junio 2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[sur la]</klr> maldikan pasta&jcirc;on <klr>[&#x2026;]</klr> oni metas <klr>[&#x2026;]</klr>
        kremdensa&jcirc;on el densigita dol&ccirc;a lakto, muelitajn kremigitajn migdalojn,
        sekvinberojn, poste kunvolvas, <tld/>etas sur malforta fajro
        <fnt>
          <aut>M&oacute;r J&oacute;kai, trad. L&aacute;szl&oacute; P&aacute;sztor	</aut>
          <vrk><url ref="http://www.verkoj.com/tradukistoj/laszlo-pasztor/la-filoj-de-la-sxtonkora-homo/">La filoj de l&#x2019; &scirc;tonkora homo</url></vrk>
          <lok>&Ccirc;ionatendanta Adamo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_s;&c_m;&c_a;&c_zh;&c_y;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>anbr&auml;unen</trd>,
    <trd>anschwitzen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>saisir</trd>,
    <trd>faire revenir</trd>
  </trdgrp>
  <trd lng="he">&lamed;&he;&shin;&het;&yod;&fmem; \<ind>&shin;&het;&fmem;</ind>\</trd>
  <trd lng="hu">megpir&iacute;t</trd>
  <trdgrp lng="ja">
    <trd>&#x7126;&#x3052;&#x76EE;&#x3092;&#x3064;&#x3051;&#x308B;  [<ind>&#x3053;&#x3052;&#x3081;&#x3092;&#x3064;&#x3051;&#x308B;</ind>]</trd>,
    <trd>&#x7126;&#x304C;&#x3059;  [<ind>&#x3053;&#x304C;&#x3059;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">bruinen<klr tip="ind"> (v.vlees bv.)</klr></trd>
  <trd lng="pl">zarumieni&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_zh;&c_a;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_zh;&c_a;&c_r;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="uk">&c_p;&c_ib;&c_d;&c_s;&c_m;&c_a;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i; (&c_z;&c_n;&c_i;&c_z;&c_u;)</trd>
</drv>

<drv mrk="rost.0pano">
  <kap><tld/>pano</kap>
  <snc mrk="rost.0pano.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Peco de <tld/>ita pano:
      <ekz>
        sin kviete konsoli per te-taso kaj varma <tld/>pano
        <fnt><aut>Saki, trad. E. Grimley-Evans</aut>, <vrk>La
        malripoz-kuracado</vrk>,
