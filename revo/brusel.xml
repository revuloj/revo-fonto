<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Brusel</rad>/o
</kap>
<drv mrk="brusel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>ZR</bib></fnt>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="brusel.0o.urb">
    <dif>
      <ref tip="super" cel="urb.0o">Urbo</ref> en centra
      <ref tip="malprt" cel="belg.B0ujo">Belgujo</ref> (4&ring;20'E,
      50&ring;50'N):
      <ekz>
        kaj tiuj kiuj lo&gcirc;as en Litomi&scirc;lo nur apogu tiujn,
        kiuj lo&gcirc;as
        en <tld/>o kaj Nov-Jorko
        <fnt><aut>Walter &Zdot;ELAZNY</aut>:
        <vrk>Pri la rilatoj inter inteligencio kaj plebo en la movado
        a&ubreve; intelektulo
        inter plebanoj kaj nuda inter lupoj</vrk>,
        <bib>LOdE</bib>,
        <lok>n-ro 04/2000</lok> </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="brusel.0o.reg">
    <dif>
      La meza dulingva regiono de <ref tip="malprt"
      cel="belg.B0ujo">Belgujo</ref>,
      konsistanta el 19 <ref tip="vid" cel="komun.0umo">komunumoj</ref>,
      inter kiuj
      la plej grava estas la urbo <ref tip="prt"
      cel="brusel.0o.urb">Bruselo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_B;&c_r;&c_u;&c_s;&c_e;&c_l;&c_mol;</trd>
  <trd lng="bg">&c_B;p&c_ju;&c_k;ce&c_l;</trd>
  <trd lng="ca">Brusse&#320;les</trd>
  <trd lng="cs">Brusel</trd>
  <trd lng="da">Bryssel</trd>
  <trd lng="de">Br&uuml;ssel</trd>
  <trd lng="en">Brussels</trd>
  <trd
  lng="el">&Beta;&rho;&ypsilon;&xi;&epsilon_acute;&lambda;&lambda;&epsilon;&sigma;</trd
  >
  <trd lng="es">Bruselas</trd>
  <trd lng="fi">Bryssel</trd>
  <trd lng="fr">Bruxelles</trd>
  <trd lng="gd">an <ind>Bhruis&eacute;il</ind></trd>
  <trd lng="hu">Br&uuml;sszel</trd>
  <trd lng="is">Brussel</trd>
  <trd lng="it">Bruxelles</trd>
  <trd lng="lat">Bruxella</trd>
  <trd lng="lv">Brisele</trd>
  <trd lng="nl">Brussel</trd>
  <trd lng="pl">Bruksela</trd>
  <trd lng="pt">Bruxelas</trd>
  <trd lng="ru">&c_B;p&c_ju;cce&c_l;&c_mol;</trd>
  <trd lng="sk">Brusel</trd>
  <trd lng="sl">Bruselj</trd>
  <trd lng="sv">Bryssel</trd>
  <trd lng="tr">Br&uuml;ksel</trd>
</drv>
</art>
