<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nok.xml,v 1.11 2022/03/24 10:41:40 revo Exp $">
<kap>
    <rad>nok</rad>/oj 
</kap>
<drv mrk="nok.0oj">
  <kap><tld/>oj</kap>
<snc mrk="nok.0oj.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="lst" cel="mangx.0ajxo" lst="voko:man&gcirc;a&jcirc;oj">Man&gcirc;a&jcirc;o</ref> de 
      pasto<ref tip="super" cel="bul.0o">bul</ref>etoj ofte ovalaj,
      kuiritaj en akvo:
      <ekz>
        sinkintaj en bolantan akvon <tld/>oj revenas flosi sur 
        la akva surfaco
        kaj tiam  estas pretaj;
      </ekz>
      <ekz>
        <tld/>oj de terpomoj
        <fnt><vrk><url ref="http://www.youtube.com/watch?v=x2Ao26_LuKA">
              video kiu montras en Esperanto metodon por elfari nokojn
              el terpomoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <ind>&scirc;vabaj <tld/>oj</ind>;
        <trd lng="de">Schupfnudeln</trd>
      </ekz>
      <ekz>
        <tld/>oj kun brindzo
        <fnt>
          <aut>Uwe Stecher</aut>,
          <vrk>Esperanto aktuell, Ekskurso al Bratislavo</vrk>,
          <lok>2016/3, p.8</lok>
        </fnt>;
      </ekz>
      <ekz>
        ru&gcirc;betaj <tld/>oj
        <fnt>
          <vrk><url ref="http://apetito.ikso.net/recepto/rugxbetaj-nokoj">
              Ru&gcirc;betaj nokoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">noky</trd>
  <trdgrp lng="de">
    <trd>Nocken</trd>,
    <trd>Nockerln</trd>
  </trdgrp>
  <trd lng="en">Gnocchi</trd>
  <trd lng="es">&Ntilde;oqui</trd>
  <trd lng="fr">gnocchis</trd>
  <trdgrp lng="hu">
    <trd>galuska</trd>,
    <trd>nokli</trd>
  </trdgrp><trd lng="it">Gnocchi</trd>
  <trdgrp lng="pl">
    <trd>kopytka</trd>,
    <trd>gnocchi <klr>(w wersji w&lstroke;oskiej)</klr></trd>
  </trdgrp>
  <trd lng="pt">Nhoque</trd>
  <trd lng="sk">halu&scaron;ky</trd>
  <trd lng="uk">&#x433;&#x430;&#x43B;&#x443;&#x448;&#x43A;&#x438;</trd>
</drv>
</art>

