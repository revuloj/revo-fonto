<kap>
  <ofc>*</ofc>
  <rad>pa&scirc;t</rad>/i <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="pasxt.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pasxt.0i.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Konduki brutojn sur kampo, por ke ili man&gcirc;u herbon:
      <ekz>
        sur tiuj &ccirc;i vastaj kaj herbori&ccirc;aj kampoj <tld/>as sin grandaj brutaroj
        <fnt>
          <bib>F</bib>
          <lok>&FE; 34</lok>
        </fnt>;
      </ekz>
      <ekz>
        venis <nom>Ra&hcirc;el</nom> kun la &scirc;afoj <klr>[kaj]</klr> pa&scirc;tis ilin
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 29:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        miajn fratojn mi ser&ccirc;as <klr>[&#8230;]</klr>, kie ili <tld/>as
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 37:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Dio <klr>[&#8230;]</klr> <tld/>is min de mia naski&gcirc;o &gcirc;is la nuna tago
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 48:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la verda herbejo ekster la urbo <tld/>is sin &ccirc;evaloj
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la restanta parto de la parko pluraj tutsioj <klr>[&#8230;]</klr>
        <tld/>is siajn bovinojn
        <fnt>
          <aut>W. M. A. De Smet</aut>,
          <vrk>Kiu murdis Dian Fossey?</vrk>,
          <bib>Monato</bib>,
          <lok>1995:12, p. 23a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_mol;&c_v;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="br">
    <trd>kas da beuri&ntilde;</trd>,
    <trd>peuri&ntilde; <klr tip="ind">(kas da beuri&ntilde;)</klr></trd>
  </trdgrp>
  <trd lng="ca">pasturar</trd>
  <trdgrp lng="de">
    <trd><ind>weiden</ind> lassen</trd>,
    <trd>hegen <klr>(weiden)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>(take to) <ind>pasture</ind></trd>,
    <trd>shepherd</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire <ind>paitre</ind></trd>
  </trdgrp>
  <trd lng="hu">legeltet</trd>
  <trdgrp lng="id">
    <trd><baz>angon</baz>mengangon</trd>,
    <trd><baz>gembala</baz>menggembala</trd>,
    <trd><baz>gembala</baz>menggembalakan</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#29287;&#22580;&#12408;&#36899;&#x308C;&#12390;&#x884C;&#x304F;  <pr>&#x307C;&#x304F;&#12376;&#12423;&#12358;&#12408;&#12388;&#x308C;&#12390;&#12356;&#x304F;</pr></trd>,
    <trd>&#x653E;&#29287;&#12377;&#x308B;  <pr>&#x307B;&#12358;&#x307C;&#x304F;&#12377;&#x308B;</pr></trd>,
    <trd>&#25937;&#12356;&#x5C0E;&#x304F;  <pr>&#12377;&#x304F;&#12356;&#x307F;&#12385;&#12403;&#x304F;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>weiden</trd>,
    <trd>laten <ind>grazen</ind></trd>
  </trdgrp>
  <trd lng="pl">pa&sacute;&cacute;<klr> (zwierz&eogonek;ta)</klr></trd>
  <trd lng="pt">pastorear</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_i;</trd>
  <trd lng="uk">&#x43F;&c_a;&c_s;&c_t;&c_i;</trd>
  <trd lng="zh">&#x653E;&#29287; <pr>f&#xE0;ngm&#xF9;</pr></trd>
</drv>

<drv mrk="pasxt.0ataro">
  <kap><tld/>ataro</kap>
  <snc mrk="pasxt.0ataro.AGR">
    <dif>
      <ref tip="dif" cel="brut.0aro">Brutaro</ref>:
      <ekz>
        Li elmetis <klr>[&#8230;]</klr> iliajn <tld/>atarojn al fulmo
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 78:48</lok>
        </fnt>;
      </ekz>
      <ekz>
        ilia tuta <tld/>ataro estas dispelita
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 10:21</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_t;&c_a;&c_k;</trd>
  <trdgrp lng="br">
    <trd>chatal</trd>,
    <trd>tropell</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>ramat</trd>,
    <trd>bestiar</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Herde</trd>,
    <trd>Viehherde</trd>
  </trdgrp><trdgrp lng="fr">
    <trd>b&eacute;tail</trd>,
    <trd>troupeau</trd>
  </trdgrp>
  <trd lng="id">ternak</trd>
  <trdgrp lng="ja">
    <trd>&#x5BB6;&#x755C;&#x306E;&#x7FA4;&#x308C; 
    <pr>&#x304B;&#12385;&#x304F;&#x306E;&#12416;&#x308C;</pr></trd>,
    <trd>&#x4FE1;&#x5F92;  <pr>&#12375;&#12435;&#12392;</pr></trd>
  </trdgrp>
  <trd lng="pl">stado<klr> (pas&aogonek;ce si&eogonek;)</klr></trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_d;&c_o;</trd>
  <trdgrp lng="uk">
    <trd>&c_ch;&c_je;&c_r;&c_je;&c_d;&c_a;</trd>,
    <trd>&#x43E;&c_t;&c_a;&c_r;&c_a;</trd>,
    <trd>&#x43F;&c_a;&c_s;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5BB6;&#x755C; <pr>ji&amacron;ch&#xF9;</pr></trd>,
    <trd>&#x725B;&#x7F8A; <pr>ni&#xFA;y&#xE1;ng</pr></trd>,
    <trd>&#29298;&#x755C; <pr>sh&emacron;ngch&#xF9;</pr></trd>,
    <trd>&#x755C; <pr>ch&#xF9;</pr></trd>,
    <trd>&#x7FA4; <pr>q&#xFA;n</pr></trd>,
    <trd>&#22131; <pr>y&#x1D4;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pasxt.0ejo">
  <kap><tld/>ejo, <var><kap><tld/>i&gcirc;ejo</kap></var></kap>
  <snc mrk="pasxt.0ejo.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Loko kie oni <tld/>as brutojn, herbejo:
      <ekz>
        ser&ccirc;i <tld/>ejon por siaj &scirc;afoj
        <fnt>
          <bib>MT</bib>
          <lok>I. Kroniko 4:39</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;ejo por brutaroj
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 32:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        eksonis sur la <tld/>ejo plenda kanto de pa&scirc;tisto
        <fnt>
          <bib>IKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        la brutaro mem revenas de la <tld/>ejoj, kvaza&ubreve; proksimi&gcirc;us la vespero
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel la <tld/>isto devigas bovinojn iri al la <tld/>ejo, tiel
        anka&ubreve; maljuna&gcirc;o kaj morto devigas la vivojn iri el la esta&jcirc;oj
        <fnt><aut>trad. T. T. Anuruddha</aut>, <vrk>La Vojo al Nirvano
        a&ubreve; la Dhammapado, 10:135</vrk>, <lok>B&#x1ED3;-T&aacute;t-Csoma-Instituto por
        Budhologio, 1973</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_sh;&c_a;</trd>
  <trd lng="br">peurvan</trd>
  <trdgrp lng="ca">
    <trd>pastura</trd>,
    <trd>pasturatge</trd>
  </trdgrp>
  <trd lng="de">Weide</trd>
  <trdgrp lng="fr">
    <trd>p&acirc;turage</trd>,
    <trd>p&acirc;ture<klr> (p&acirc;turage)</klr></trd>
  </trdgrp>
  <trd lng="hu">legel&odblac;</trd>
  <trdgrp lng="ja">
    <trd>&#29287;&#22580;  <pr>&#x307C;&#x304F;&#12376;&#12423;&#12358;</pr></trd>,
    <trd>&#29287;&#33609;&#22320;  <pr>&#x307C;&#x304F;&#x305D;&#12358;&#12385;</pr></trd>
  </trdgrp>
  <trd lng="nl">weide</trd>
  <trd lng="en">pasture</trd>
  <trd lng="id"><ind>padang</ind> rumput</trd>
  <trd lng="pl">pastwisko</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_b;&c_i;&c_shch;&c_je;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_a;&c_s;&#x43E;&c_v;&c_i;&c_s;&#x44C;&#x43A;&#x43E;</trd>,
    <trd>&#x43F;&c_a;&c_s;&#x43E;&c_v;&c_i;&c_shch;&c_je;</trd>,
    <trd>&c_v;&c_i;&#x43F;&c_a;&c_s;</trd>,
    <trd>&#x43F;&c_a;&c_s;&c_t;&#x43E;&c_v;&c_je;&#x43D;&#x44C;</trd>,
    <trd>&#x43F;&c_a;&c_s;&c_t;&c_ib;&c_v;&c_je;&#x43D;&#x44C;</trd>,
    <trd>&#x43F;&c_a;&c_s;&c_t;&c_ib;&c_v;&#x43D;&c_i;&#x43A;</trd>,
    <trd>&c_v;&c_i;&c_g;&c_ib;&#x43D;</trd>,
    <trd>(&c_g;&c_ib;&c_r;&c_s;&#x44C;&#x43A;&c_je;) &#x43F;&#x43E;&#x43B;&#x43E;&#x43D;&c_i;&#x43D;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#33609;&#x573A; <pr>c&#x1CE;och&#x1CE;ng</pr></trd>,
    <trd>&#33609;&#22580; <pr>c&#x1CE;och&#x1CE;ng</pr></trd>,
    <trd>&#29287;&#33609; <pr>m&#xF9;c&#x1CE;o</pr></trd>,
    <trd>&#29287;&#x573A; <pr>m&#xF9;ch&#x1CE;ng</pr></trd>,
    <trd>&#29287;&#22580; <pr>m&#xF9;ch&#x1CE;ng</pr></trd>,
    <trd>&#29287;&#x533A; <pr>m&#xF9;q&#x16B;</pr></trd>,
    <trd>&#29287;&#21312; <pr>m&#xF9;q&#x16B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pasxt.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="pasxt.0igxi.AGR">
    <dif>
      Sin <tld/>i, sin nutri, man&gcirc;ante herbojn, glanojn kaj
      similajn:
      <ekz>
        sep bovinoj <klr>[&#8230;]</klr> <tld/>i&gcirc;as en la kanejo
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 41:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;afidoj <tld/>i&gcirc;os en bona ordo
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 5:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vila&gcirc;anoj transiras en la <tld/>istajn dometojn kaj lasas siajn brutojn
        <tld/>i&gcirc;i sur la alta&jcirc;oj
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_mol;&c_v;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trd lng="br">peuri&ntilde;<klr> (v.g.)</klr></trd>
  <trd lng="ca">pasturar</trd>
  <trd lng="de">weiden</trd>
  <trd lng="fr">paitre<klr> (intr.)</klr></trd>
  <trd lng="hu">legel</trd>
  <trd lng="ja">&#33609;&#12434;&#x98DF;&#12409;&#x308B;  <pr>&#x304F;&#12373;&#12434;&#x305F;&#12409;&#x308B;</pr></trd>
  <trd lng="nl">grazen</trd>
  <trd lng="pl">pa&sacute;&cacute; si&eogonek;</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_i;&c_s;&c_mol;</trd>
  <trd lng="uk">&#x43F;&c_a;&c_s;&c_t;&c_i;&c_s;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#21507;&#33609; <pr>ch&#x12B;c&#x1CE;o</pr></trd>,
    <trd>&#29287;&#x653E; <pr>m&#xF9;f&#xE0;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pasxt.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="pasxt.0isto.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Tiu, kies okupo estas <tld/>ado:
      <ekz>
        <nom>Habel</nom> fari&gcirc;is &scirc;af<tld/>isto
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 4:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne estu malpaco inter mi kaj vi kaj inter miaj <tld/>istoj kaj viaj <tld/>istoj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 13:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la Eternulo estas mia <tld/>isto, mi mankon ne
        havos
        <fnt><bib>MT</bib>, <lok>&Psa; 23:1</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>isto gaje jodlis en la valo
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        imperiestra porko<tld/>isto
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tisto de porkoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="gauxcx.0o">ga&ubreve;&ccirc;o</ref>,
      <ref cel="vaker.0o">vakero</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_t;&c_u;&c_h;</trd>
  <trdgrp lng="br">
    <trd>bugul</trd>,
    <trd>maesaer</trd>
  </trdgrp>
  <trd lng="ca">pastor</trd>
  <trd lng="de">Hirte</trd>
  <trdgrp lng="fr">
    <trd>berger</trd>,
    <trd>p&acirc;tre</trd>
  </trdgrp>
  <trd lng="hu">p&aacute;sztor</trd>
  <trdgrp lng="ja">
    <trd>&#29287;&#32773;  <pr>&#x307C;&#x304F;&#12375;&#12419;</pr></trd>,
    <trd>&#29287;&#x7AE5;  <pr>&#x307C;&#x304F;&#12393;&#12358;</pr></trd>,
    <trd>&#x7F8A;&#x98FC;&#12356;  <pr>&#12402;&#12388;&#12376;&#x304B;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">herder</trd>
  <trd lng="en">herder, shepherd</trd>
  <trdgrp lng="id">
    <trd><baz>angon</baz>pengangon</trd>,
    <trd><baz>gembala</baz>penggembala</trd>
  </trdgrp>
  <trd lng="pl">pasterz</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_u;&c_h;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_a;&c_s;&c_t;&c_u;&c_h;</trd>,
    <trd>&c_ch;&c_a;&c_b;&c_a;&#x43D;</trd>,
    <trd>&c_v;&c_ib;&c_v;&c_ch;&c_a;&c_r;</trd>,
    <trd>&#x43F;&c_a;&c_s;&c_t;&c_i;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x500C; <pr>gu&amacron;n</pr></trd>,
    <trd>&#29287;&#x4EBA; <pr>m&#xF9;r&#xE9;n</pr></trd>,
    <trd>&#29287;&#x7F8A;&#x4EBA; <pr>m&#xF9;y&#xE1;ngr&#xE9;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pasxt.somer0ejo">
  <kap>somer<tld/>ejo</kap>
  <snc mrk="pasxt.somer0ejo.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Somertempe uzata <tld/>ejo, precipe tia, kia
      trovi&gcirc;as en alta montaro;
      <ref tip="sin" cel="alp.0o.AGR">alpo<sncref/></ref>:
      <ekz>
        vi garde zorgu pri la kvindek &scirc;afinoj en la somer<tld/>ejo
        <fnt>
          <aut>-, trad. B. Ragnarsson</aut>,
          <vrk>Rakonto pri Hrafnkel</vrk>,
          <lok>en: Islandaj Pravo&ccirc;oj, 1964</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la somer<tld/>ejo <klr>[nun]</klr> konstrui&gcirc;as konstantaj vila&gcirc;oj el betono
        <fnt>
          <bib>MD14</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_sh;&c_a; <klr>(&c_l;&c_je;&c_t;&c_n;&c_ja;&c_ja;)</klr></trd>
  <trdgrp lng="de">
    <trd>Sommerweide</trd>,
    <trd>Alm</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>alpage</trd>,
    <trd>p&acirc;turage estival</trd>
  </trdgrp>
  <trd lng="id"><ind>padang</ind> rumput musim panas</trd>
  <trdgrp lng="zh">
    <trd>&#x9AD8;&#x5C71;&#29287;&#x573A; <pr>g&amacron;osh&amacron;nm&#xF9;ch&#x1CE;ng</pr></trd>,
    <trd>&#x9AD8;&#x5C71;&#29287;&#22580; <pr>g&amacron;osh&amacron;nm&#xF9;ch&#x1CE;ng</pr></trd>
  </trdgrp>
</drv>
</art>
