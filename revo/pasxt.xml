<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pa&scirc;t</rad>/i
</kap>
<drv mrk="pasxt.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Konduki brutojn sur kampo, por ke ili man&gcirc;u herbon.
    </dif>
  </snc>
  <trd lng="de">weiden lassen</trd>
  <trdgrp lng="nl">
    <trd>weiden</trd>,
    <trd>laten <ind>grazen</ind></trd>
  </trdgrp>
  <trd lng="en">pasture</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_i;</trd>
</drv>

<drv mrk="pasxt.0ataro">
  <kap><tld/>ataro<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Brutaro.
    </dif>
  </snc>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_d;&c_o;</trd>
</drv>

<drv mrk="pasxt.0igxejo">
  <kap><tld/>i&gcirc;ejo, <tld/>ejo<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Loko kie oni <tld/>as brutojn, herbejo.
    </dif>
  </snc>
  <trd lng="de">Weide</trd>
  <trd lng="nl">weide</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_b;&c_i;&c_shch;&c_je;</trd>
</drv>

<drv mrk="pasxt.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Sin <tld/>i, sin nutri, man&gcirc;ante herbojn, glanojn ks.
    </dif>
  </snc>
  <trd lng="de">weiden</trd>
  <trd lng="nl">grazen</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_i;&c_s;&c_mol;</trd>
</drv>

<drv mrk="pasxt.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Brutgardisto.
    </dif>
  </snc>
  <trd lng="de">Hirte</trd>
  <trd lng="nl">herder</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_u;&c_h;</trd>
</drv>
</art>
