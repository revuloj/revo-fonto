<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>eter</rad>/o
    </kap>
    <drv mrk="eter.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc>
        <uzo tip="fak">FIZ</uzo>
        <dif>
          Senpeza elasta substanco, kiun oni supozis penetri &ccirc;iujn
          korpojn kaj transsendi la diversajn formojn de energio.  
        </dif>
        <adm>
          La difinoj estas reverkendaj. Mi nur sxangxis "supozas" al "supozis"...
          <aut>MB</aut>
        </adm>
      </snc>
      <snc mrk="eter.0o.klaso">
        <uzo tip="fak">KEM</uzo>
        <dif>
          Ano de la klaso de organikaj kombina&jcirc;oj konsistantaj el du
          <ref cel="hidrok.0o">hidrokarbidaj</ref>
          <ref cel="radika1.0o.KEM">radikaloj</ref>, interligitaj per
          atomo de oksigeno: <frm>R-O-R</frm>
          <ekz>
            (<ind>simpla <tld/>o</ind>),
            <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_t;&c_o;&c_j; &c_e;&c_f;&c_i;&c_r;</trd>
          </ekz>
          a&ubreve; per atomgrupo <frm>-OCO-</frm>
          <ekz>
            (<ind>malsimpla <tld/>o</ind>).
            <trd lng="ru">&c_s;&c_l;&c_o;&c_zh;&c_n;&c_y;&c_j; &c_e;&c_f;&c_i;&c_r;</trd>
          </ekz>
        </dif>
      </snc>
      <snc mrk="eter.0o.MED">
        <uzo tip="fak">KEM</uzo>
        <uzo tip="fak">MED</uzo>
        <dif>
          La plej vaste konata <tld/>o,<sncref ref="eter.0o.klaso"/>,
          <frm>C<sub>2</sub>H<sub>5</sub>OC<sub>2</sub>H<sub>5</sub></frm>
          (dietil<tld/>o), tre facile vapori&gcirc;anta flua&jcirc;o, anestezilo.
        </dif>
      </snc>
      <trd lng="fr">&eacute;ther</trd>
      <trd lng="ru">&c_e;&c_f;&c_i;&c_r;</trd>
      <trd lng="sv">eter</trd>
    </drv>
  </art>
