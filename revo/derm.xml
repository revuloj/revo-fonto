<art mrk="$Id: derm.xml,v 1.24 2025/01/25 14:24:35 revo Exp $">
<kap>
<rad>derm</rad>/o
</kap>
<drv mrk="derm.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      La suba <ref tip="super" cel="tavol.0o.KOMUNE">tavolo</ref>
      de la <ref tip="malprt" cel="hauxt.0o.ANA">ha&ubreve;to</ref>:
      <ekz>
        dum la malsano <klr>(akara boreliozo)</klr> precipe estas atakataj la <tld/>o
        (per migranta eritemo), artikoj, nerva sistemo, koro
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2007/009243.php">Epidemiaj sekvoj en E&ubreve;ropo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la viruso de rabio el la salivo atakas la vundon kaj reprodukti&gcirc;as en la <tld/>o
        kaj muskoloj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2008/009550.php">Batalo kontra&ubreve; mortiga malsano</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="epider.0o">epidermo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_r;&c_m;&c_a;</trd>
  <trd lng="ca">derma</trd>
  <trdgrp lng="cs">
    <trd>derma</trd>,
    <trd>k&uring;&zcaron;e</trd>,
    <trd>poko&zcaron;ka</trd>,
    <trd>&scaron;k&aacute;ra</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Lederhaut</trd>,
    <trd>Unterhaut</trd>
  </trdgrp>
  <trd lng="en">dermus</trd>
  <trd lng="es">dermis</trd>
  <trd lng="fr">derme</trd>
  <trd lng="he">&#x5E9;&#x5DB;&#x5D1;&#x5EA; &#x5E2;&#x5D5;&#x5E8;</trd>
  <trd lng="hu">irha</trd>
  <trd lng="it">derma</trd>
  <trd lng="ja">&#x771F;&#x76AE;  <pr>&#12375;&#12435;&#12404;</pr></trd>
  <trd lng="pl">sk&oacute;ra</trd>
  <trdgrp lng="pt">
    <trd>derme</trd>,
    <trd>derma</trd>
  </trdgrp>
  <trd lng="ro">piele</trd>
  <trd lng="ru">&c_d;&c_je;&c_r;&c_m;&c_a;</trd>
  <trd lng="sk">ko&zcaron;a</trd>
  <trdgrp lng="uk">
    <trd>&c_sh;&#x43A;&c_ib;&c_r;&c_a;</trd>,
    <trd>&c_d;&c_je;&c_r;&#x43C;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#x5DE9;&#x819C; <pr>g&#x1D2;ngm&#xF2;</pr></trd>
</drv>
</art>
