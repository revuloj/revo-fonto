<vortaro>
<art mrk="$Id$">
<kap>
<rad>derm</rad>/o
</kap>
<drv mrk="derm.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      La suba tavolo de la ha&ubreve;to.
    </dif>
    <refgrp tip="vid">
      <ref cel="epider.0o">epidermo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_r;&c_m;&c_a;</trd>
  <trd lng="ca">derma</trd>
  <trdgrp lng="cs">
    <trd>derma</trd>,
    <trd>kůže</trd>,
    <trd>pokožka</trd>,
    <trd>škára</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Lederhaut</trd>,
    <trd>Unterhaut</trd>
  </trdgrp>
  <trd lng="en">dermus</trd>
  <trd lng="es">dermis</trd>
  <trd lng="fr">derme</trd>
  <trd lng="hu">irha</trd>
  <trd lng="it">derma</trd>
  <trd lng="pl">sk&oacute;ra</trd>
  <trdgrp lng="pt">
    <trd>derme</trd>,
    <trd>derma</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_je;&c_r;&c_m;&c_a;</trd>
  <trd lng="sk">koža</trd>
</drv>
</art>
