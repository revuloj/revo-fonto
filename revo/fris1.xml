<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>fris</rad>/o
</kap>
<drv mrk="fris1.0o">
  <kap><tld/>o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Homo kun <tld/>a nacieco.
    </dif>
  </snc>
  <ref tip="hom" cel="fris.0o">friso</ref>
  <trd lng="be">&c_f;&c_r;&c_y;&c_z;</trd>
  <trd lng="de">Friese</trd>
  <trd lng="fr">Frison</trd>
  <trd lng="nl">Fries</trd>
  <trd lng="ru">&c_f;&c_r;&c_i;&c_z;</trd>
 </drv>
<drv mrk="fris1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj a&ubreve; ilia nacieco.
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_r;&c_y;&c_z;&c_s;&c_k;&c_ib;</trd>
  <trd lng="de">friesisch</trd>
  <trd lng="fr">frison</trd>
  <trd lng="nl">Fries</trd>
  <trd lng="ru">&c_f;&c_r;&c_i;&c_z;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
<drv mrk="fris1.F0ujo">
  <kap><tld lit="F"/>ujo</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="fris1.F0ujo.prov">
    <dif>
     <ref tip="super" cel="provin.0o">Provinco</ref> en norda <ref tip="malprt"
     cel="nederl.0o">Nederlando</ref>, anka&ubreve; nomata okcidenta <tld lit="F"/>ujo.
    </dif>
  </snc>
   <snc mrk="fris1.F0ujo.reg">
    <dif>
     Regiono apud la Norda Maro, en norda <ref tip="malprt" cel="nederl.0o">Nederlando</ref>
     (okcidenta <tld lit="F"/>ujo) kaj en nord-okcidenta <ref tip="malprt"
     cel="german1.G0ujo">Germanujo</ref>.
    </dif>
  </snc>
  <ref tip="sin" cel="frisi.0o">Frisio</ref>
  <trd lng="be">&c_F;&c_r;&c_y;&c_z;&c_ib;&c_ja;</trd>
  <trd lng="da">Frisland</trd>
  <trd lng="de">Friesland</trd>
  <trd lng="cs">Fr&iacute;zsko</trd>
  <trd lng="en">Frisia</trd>
  <trd lng="es">Frisia</trd>
  <trd lng="fr">Frise</trd>
  <trd lng="it">Frisia</trd>
  <trd lng="nl">Friesland</trd>
  <trd lng="pl">Fryzja</trd>
  <trd lng="pt">Fr&iacute;sia</trd>
  <trd lng="ru">&c_F;&c_r;&c_i;&c_z;&c_i;&c_ja;</trd>
  </drv>
  <drv mrk="fris1.F0lando">
  <kap><tld lit="F"/>lando</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="fris1.F0ujo">Frisujo</ref>: <ekz>pli grandajn polderojn tiel
      faritajn, ni trovas en la nordo de la lando, en la provinco <tld lit="F"/>lando
      <fnt><aut>Wouter PILGER</aut>:
      <vrk>Nederlando kaj la maro</vrk>
      </fnt>.
      </ekz>
    </dif>
  </snc>
  </drv>
</art>

