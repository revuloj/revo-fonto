<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: deci.xml,v 1.5 2019/06/30 18:19:02 revo Exp $">
<kap>
    <rad>deci</rad>- <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="deci.0">
  <kap><tld/>-</kap>
  <mlg>d</mlg>
  <gra><vspec>prefikso</vspec></gra>
  <snc mrk="deci.0.MEZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      &leftquot;Dekono da&rightquot;, 10<sup>-1</sup>
      en la Internacia Sistemo de unuoj (SI):
      <ekz>
        sufi&ccirc;as unu nigre vestita rolanto, kiu dignamiene kaj mildageste demetas la ujon
        en etan truon malmultajn <tld/>metrojn sub la tersurfaco
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2014/011167.php"
              >&Ccirc;erko&ccirc;are mono&scirc;pare</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; la rabadoj li trinkis 1&#8239;-&#8239;2 <tld/>litrojn da viskio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=466020"
              >Attila Ambrus</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="deka.0">deka-</ref>,
       <ref cel="decima.0a">decimala</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_c;&c_y;-</trd>
  <trd lng="bg">&c_d;&c_je;&c_c;&c_i;-</trd>
  <trd lng="ca">deci-</trd>
  <trd lng="de">Dezi-</trd>
  <trd lng="en">deci-</trd>
  <trd lng="es">dec&iacute;-</trd>
  <trd lng="fa">&a_d;&a_s;&f_y;</trd>
  <trd lng="fr">d&eacute;ci-</trd>
  <trd lng="hu">deci-</trd>
  <trd lng="it">deci-</trd>
  <trd lng="nl">deci-</trd>
  <trd lng="no">desi-</trd>
  <trd lng="pl">decy-</trd>
  <trd lng="pt">dec&iacute;-</trd>
  <trd lng="ru">&c_d;&c_je;&c_c;&c_i;-</trd>
  <trd lng="sk">deci-</trd>
  <trd lng="sv">deci-</trd>
</drv>
</art>

