<vortaro>
<art mrk="$Id: ekskur.xml,v 1.32 2021/06/01 19:53:18 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ekskurs</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="ekskur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="ekskur.0o.TUR">
  <dif>
    Longa plezura a&ubreve; esplora promenado:
    <ekz>scienca, enmonta, geologia <tld/>o.</ekz>
  </dif>
  </snc>
  <trd lng="af">reis</trd>
  <trd lng="am">&#x1309;&#x12DE;</trd>
  <trd lng="ar">&#x631;&#x62D;&#x644;&#x629;</trd>
  <trd lng="az">s&#x259;f&#x259;r</trd>
  <trd lng="be">&c_e;&c_k;&c_s;&c_k;&c_u;&c_r;&c_s;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_je;&c_k;&c_s;&c_k;&c_u;&c_r;&c_z;&c_i;&c_ja;</trd>
  <trd lng="bn">&#x99F;&#x9CD;&#x9B0;&#x9BF;&#x9AA;</trd>
  <trd lng="bo">&#xF63;&#xF9F;&#xF0B;&#xF66;&#xF90;&#xF7C;&#xF62;</trd>
  <trdgrp lng="ca">
    <trd>excursi&oacute;</trd>,
    <trd>sortida</trd>,
    <trd>escapada</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>p&#x159;echodn&#xE9; odbo&#x10D;en&#xED; ze sledu &#x159;e&#x10D;i</trd>,
    <trd>v&yacute;let</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Ausflug</trd>,
      <trd>Exkursion</trd>
   </trdgrp>
  <trd lng="en">excursion</trd>
  <trd lng="es">excursi&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>excursion</trd>,
    <trd>randonn&eacute;e</trd>
  </trdgrp>
  <trd lng="hu">kir&aacute;ndul&aacute;s</trd>
  <trd lng="hy">&#x578;&#x582;&#x572;&#x565;&#x582;&#x578;&#x580;&#x578;&#x582;&#x569;&#x575;&#x578;&#x582;&#x576;</trd>
  <trd lng="ne">&#x92F;&#x93E;&#x924;&#x94D;&#x930;&#x93E;</trd>
  <trdgrp lng="nl">
   <trd>excursie</trd>,
   <trd>uitstap</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wycieczka</trd>,
    <trd>wyprawa</trd>
  </trdgrp>
  <trd lng="pt">excurs&atilde;o</trd>
  <trd lng="ru">&c_e;&c_k;&c_s;&c_k;&c_u;&c_r;&c_s;&c_i;&c_ja;</trd>
  <trd lng="sk">prechodn&#xE9; odbo&#x10D;enie zo sledu vec&#xED;</trd>
  <trd lng="sq">udh&#xEB;tim</trd>
  <trd lng="sv">utflykt</trd>
</drv>

<drv mrk="ekskur.0i">
  <kap><tld/>i</kap>
  <snc mrk="ekskur.0i.TUR">
    <dif>
      Fari <tld/>on, vaste <ref tip="vid" cel="promen.0i">promenadi</ref>:
      <ekz>
        ili jen vizitis Gustavon, jen skiis, jen <tld/>is piede
        <fnt><aut>S. Engholm</aut>, <vrk>Vivo vokas</vrk>, 
        <lok>1946</lok></fnt>;
      </ekz>
      <ekz>
        &Gcirc;uinte la kvietecon <klr>...</klr> de la monto Qingcheng,
        ni <tld/>u al alia vidindejo, do la ne malproksima
        akvobara&jcirc;o
        Dujiangyan
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url ref="http://es1.chinabroadcast.cn/1/2006/12/06/1@50051.htm">
          La monto Qingcheng...</url></vrk>,
          <lok>2006-12-21</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="migr.0i">migri</ref>
    </refgrp>
    <trd lng="ca">fer una <ind>excursi&oacute;</ind></trd>
    <trd lng="es">salir de excursi&oacute;n</trd>
    <trd lng="fr">randonner</trd>
    <trdgrp lng="pl">
      <trd>i&sacute;&cacute; na <ind>wycieczk&eogonek;</ind></trd>,
      <trd>i&sacute;&cacute; na <ind>wypraw&eogonek;</ind></trd>
    </trdgrp>
    <trd lng="pt">fazer excurs&atilde;o</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>j&#xED;t na v&#xFD;let</trd>,
    <trd>konat v&#xFD;let</trd>,
    <trd>vyjet si na v&#xFD;let</trd>
  </trdgrp>
  <trd lng="sk">&#xED;s&#x165; na v&#xFD;let</trd>
</drv>
</art>
