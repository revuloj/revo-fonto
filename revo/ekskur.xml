<drv mrk="ekskur.0i">
  <kap><tld/>i</kap>
  <snc mrk="ekskur.0i.TUR">
    <dif>
      Fari <tld/>on:
      <ekz>
        ili jen vizitis Gustavon, jen skiis, jen <tld/>is piede
        <fnt><aut>S. Engholm</aut>, <vrk>Vivo vokas</vrk>,
        <lok>1946</lok></fnt>;
      </ekz>
      <ekz>
        ni iros al Perkolago, nur por <tld/>i
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la grupo <tld/>is al Ardana-&ccirc;e-Luzo por viziti la ruinojn tie
        <fnt>
          <bib>ChR</bib>
          <lok>Kiu pafis al Pipulo?</lok>
        </fnt>;
      </ekz>
      <ekz>
        li kelkfoje <tld/>is por tago en <nom>Valmu</nom>
        <fnt>
          <bib>ChB</bib>
          <lok>4</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;uinte la kvietecon <klr>[&#x2026;]</klr> de la monto <nom>Qingcheng</nom>,
        ni <tld/>u al alia vidindejo, do la ne malproksima
        akvobara&jcirc;o
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url ref="http://es1.chinabroadcast.cn/1/2006/12/06/1@50051.htm">
          La monto Qingcheng...</url></vrk>,
          <lok>2006-12-21</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="migr.0i">migri</ref>
    </refgrp>
    <trd lng="be">&c_ib;&c_s;&c_mol;&c_c;&c_ib; &c_n;&c_a; <ind>&c_e;&c_k;&c_s;&c_k;&c_u;&c_r;&c_s;&c_ib;&c_ju;</ind></trd>
    <trd lng="ca">fer una <ind>excursi&oacute;</ind></trd>
    <trd lng="es">salir de excursi&oacute;n</trd>
    <trd lng="fr">randonner</trd>
    <trdgrp lng="pl">
      <trd>i&sacute;&cacute; na <ind>wycieczk&eogonek;</ind></trd>,
      <trd>i&sacute;&cacute; na <ind>wypraw&eogonek;</ind></trd>
    </trdgrp>
    <trd lng="pt">fazer excurs&atilde;o</trd>
  </snc>
  <trdgrp lng="cs">
