<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: komple4.xml,v 1.25 2019/05/09 22:10:19 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>komplez</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="komple4.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="komple4.0o.KOMUNE">
    <dif>
      Ago per kiu oni penas pla&ccirc;i al iu, helpante a&ubreve;
      farante servon
      a&ubreve; havigante profiton:
      <ekz>
        <ind>faru al mi la <tld/>on</ind> ....i<fnt>Z</fnt>;
        <trdgrp lng="es">
          <trd>tenga la <ind>bondad</ind> de</trd>,
          <trd>tenga la <ind>gentileza</ind> de</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>faites-moi le <ind>plaisir</ind> de</trd>,
          <trd>faites-moi la <ind>faveur</ind> de</trd>,
          <trd>ayez l'<ind>obligeance</ind> de</trd>
        </trdgrp>
        <trd lng="hu">tedd meg nekem azt a
        sz&iacute;vess&eacute;get</trd>
      </ekz>
      <ekz>
        <ind>por amiko <tld/>o neniam estas tro peza</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">bar&aacute;t&eacute;rt tett f&aacute;rads&aacute;g
        nincsen nehez&uuml;nkre</trd>
      </ekz>
      <ekz>
        havi <tld/>ojn por iu.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_l;&c_u;&c_g;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_l;&c_u;&c_zh;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ochota</trd>,
    <trd>&uacute;slu&zcaron;nost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gefallen</trd>,
    <trd>Freundlichkeit</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>kindness</trd>,
    <trd>courtesy</trd>,
    <trd>favor</trd>,
    <trd>good offices</trd>,
    <trd>service</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>favor</trd>,
    <trd>gentileza</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>complaisance<klr tip="ind"> (obligeance)</klr></trd>,
    <trd>obligeance</trd>,
    <trd>gentillesse<klr tip="ind"> (obligeance)</klr></trd>,
    <trd>service<klr tip="ind"> (obligeance)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&iacute;vess&eacute;g</trd>,
    <trd>kedvess&eacute;g <klr
    tip="ind">(sz&iacute;vess&eacute;g)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>welwillendheid</trd>,
    <trd>gedienstigheid</trd>,
    <trd>tegemoetkoming</trd>,
    <trd>gunst</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przys&lstroke;uga</trd>,
    <trd>uprzejmo&sacute;&cacute;</trd>,
    <trd>us&lstroke;uga</trd>
  </trdgrp>   
  <trdgrp lng="pt">
    <trd>favor</trd>,
    <trd>obs&eacute;quio</trd>,
    <trd>complac&ecirc;ncia</trd>,        
    <trd>gentileza</trd>,
    <trd>fineza</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>bun&abreve;-cuviin&#539;&abreve;</trd>,
    <trd>curtoazie</trd>,
    <trd>favoare</trd>,
    <trd>serviciu</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_l;&c_u;&c_g;&c_a;</trd>,
    <trd>&c_l;&c_ju;&c_b;&c_je;&c_z;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">ochota</trd>
</drv>

<drv mrk="komple4.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="komple4.0i.KOMUNE">
    <dif>
      Fari <tld/>on:
      <ekz>
        &ccirc;u vi <tld/>us traduki al mi tiun vorton?;
      </ekz>
      <ekz>
        <tld/>u al li per via protektado<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la revolucio postulis viktimojn por <tld/>i
        <klr>(kontentigi)</klr>
        la sangavidon de &gcirc;iaj adeptoj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vol.bon0i">bonvoli</ref>,
      <ref cel="volont">volonti</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol;
    &c_l;&c_a;&c_s;&c_k;&c_a;&c_v;&c_y;&c_m;</trd>,
    <trd>&c_z;&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
    &c_p;&c_a;&c_s;&c_l;&c_u;&c_g;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>b&yacute;t laskav&yacute;</trd>,
    <trd>b&yacute;t ochotn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gef&auml;llig sein</trd>,
    <trd>freundlich sein</trd>,
    <trd>entgegenkommen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>oblige</trd>,
    <trd>be so <ind>kind</ind> as to</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>complacer<klr tip="ind"> (contentar)</klr></trd>,
    <trd>tener la <ind>amabilidad</ind> de</trd>,
    <trd>tener la <ind>gentileza</ind> de</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire le <ind>plaisir</ind> de</trd>,
    <trd>avoir l'<ind>amabilit&eacute;</ind> de</trd>,
    <trd>avoir la <ind>gentillesse</ind> de</trd>,
    <trd>avoir l'<ind>obligeance</ind> de</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&iacute;vess&eacute;get tesz</trd>,
    <trd>kedveskedik</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>de <ind>goedheid</ind> hebben</trd>,
    <trd>ter wille zijn</trd>,
    <trd>zo goed willen zijn</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zadowala&cacute;</trd>,
    <trd>zaspokaja&cacute;</trd>,
    <trd>dogadza&cacute;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>satisface</trd>,
    <trd>alina </trd>,
    <trd>te rog</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol;
    &c_u;&c_s;&c_l;&c_u;&c_g;&c_u;</trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol;
    &c_l;&c_ju;&c_b;&c_je;&c_z;&c_n;&c_y;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>by&tcaron; l&aacute;skav&yacute;</trd>,
    <trd>by&tcaron; ochotn&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="komple4.0ema">
  <kap><tld/>ema<fnt>Z</fnt>,
  <var><kap><tld/>a<fnt>Z</fnt></kap></var></kap>
  <snc mrk="komple4.0ema.KOMUNE">
    <dif>
      Inklina fari <tld/>ojn:
      <ekz>
        <ind><tld/>ema malsa&gcirc;ulo estas pli dan&gcirc;era ol
        malamiko</ind>
        <fnt><bib>PrV</bib></fnt>
        <trd lng="hu">jobb egy okos ellens&eacute;g, mint egy ostoba
        j&oacute;bar&aacute;t</trd>
        <trd
        lng="ru">&c_u;&c_s;&c_l;&c_u;&c_zh;&c_l;&c_i;&c_v;&c_y;&c_j;
        &c_d;&c_u;&c_r;&c_a;&c_k; &c_o;&c_p;&c_a;&c_s;&c_n;&c_je;&c_je;
        &c_v;&c_r;&c_a;&c_g;&c_a;</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="serv">servema</ref>,
      <ref cel="indulg">indulga</ref>.
    </refgrp>
  </snc>
  <trd
  lng="be">&c_p;&c_a;&c_s;&c_l;&c_u;&c_zh;&c_l;&c_ib;&c_v;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>koplezantn&iacute;</trd>,
    <trd>laskav&yacute;</trd>,
    <trd>ochotn&yacute;</trd>,
    <trd>&uacute;slu&zcaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gef&auml;llig</trd>,
    <trd>freundlich</trd>,
    <trd>entgegenkommend</trd>,
    <trd>zuvorkommend</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>kindly</trd>,
    <trd>benevolent</trd>,
    <trd>complaisant</trd>,
    <trd>courteous</trd>,
    <trd>friendly</trd>,
    <trd>good natured</trd>,
    <trd>obliging</trd>,
    <trd>willing</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>amable</trd>,
    <trd>complaciente</trd>,
    <trd>obsequioso</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>obligeant</trd>,
    <trd>complaisant<klr tip="ind"> (obligeant)</klr></trd>,
    <trd>serviable</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>el&odblac;z&eacute;keny</trd>,
    <trd>udvarias</trd>,
    <trd>rendes<klr tip="ind"> (el&odblac;z&eacute;keny)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>welwillend</trd>,
    <trd>toeschietelijk</trd>,
    <trd>bereidwillig</trd>,
    <trd>schikkelijk</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dobroczynny</trd>,
    <trd>grzeczny</trd>,
    <trd>uczynny</trd>,
    <trd>us&lstroke;u&zdot;ny</trd>,
    <trd>&zdot;yczliwy</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>caritabil</trd>,
    <trd>politicos</trd>,
    <trd>obligatoriu</trd>,
    <trd>util</trd>,
    <trd>serviabil</trd>,
    <trd>amabil</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_l;&c_u;&c_zh;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_l;&c_ju;&c_b;&c_je;&c_z;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>l&aacute;skav&yacute;</trd>,
    <trd>ochotn&yacute;</trd>,
    <trd>&uacute;slu&zcaron;n&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="komple4.0eal">
  <kap><tld/>e al</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="komple4.0eal.KOMUNE">
    <dif>
      Pro komplezemo al..., helpeme al...:
      <ekz>
        la sinjoro administranto faros siavice multon <tld/>e
        al la vila&gcirc;estro
        <fnt>
          <aut>B. N&ecaron;mcov&aacute;, trad. V. Tobek kaj K.
          Proch&aacute;zka</aut>,
          <vrk><url ref="https://eo.wikisource.org/wiki/Avineto/XIV.">
          Avineto</url></vrk>,
          <lok>[sen dato]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">par &eacute;gard pour</trd>
  <trd lng="pl">przez grzeczno&sacute;&cacute; dla</trd>     
  <trd lng="ro">ca o favoare</trd>
</drv>
</art>
