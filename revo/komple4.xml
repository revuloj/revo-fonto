<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>komplez</rad>/o
</kap>
<drv mrk="komple4.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Ago per kiu oni penas pla&ccirc;i al iu, helpante a&ubreve; farante servon
      a&ubreve; havigante profiton:
      <ekz>
        <ind>faru al mi la <tld/>on</ind> ...i<fnt>Z</fnt>;
        <trdgrp lng="fr">
          <trd>faites-moi le <ind>plaisir</ind> de</trd>,
          <trd>faites-moi la <ind>faveur</ind> de</trd>,
          <trd>ayez l'<ind>obligeance</ind> de</trd>
      </trdgrp>
      </ekz>
      <ekz>
        por amiko <tld/>o neniam estas tro peza
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        havi <tld/>ojn por iu.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_l;&c_u;&c_g;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_l;&c_u;&c_zh;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gefallen</trd>,
    <trd>Freundlichkeit</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>kindness</trd>,
    <trd>courtesy</trd>,
    <trd>favor</trd>,
    <trd>good offices</trd>,
    <trd>service</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>complaisance<klr tip="ind"> (obligeance)</klr></trd>,
    <trd>obligeance</trd>,
    <trd>gentillesse<klr tip="ind"> (obligeance)</klr></trd>
  </trdgrp>
  <trd lng="nl">welwillendheid</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_l;&c_u;&c_g;&c_a;</trd>,
    <trd>&c_l;&c_ju;&c_b;&c_je;&c_z;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="komple4.0ema">
  <kap><tld/>a<fnt>Z</fnt>, <tld/>ema<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Inklina fari <tld/>ojn:
      <ekz>
        <ind><tld/>ema malsa&gcirc;ulo estas pli dan&gcirc;era ol malamiko</ind>
        <fnt><bib>PrV</bib></fnt>
        <trd lng="ru">&c_u;&c_s;&c_l;&c_u;&c_zh;&c_l;&c_i;&c_v;&c_y;&c_j; &c_d;&c_u;&c_r;&c_a;&c_k; &c_o;&c_p;&c_a;&c_s;&c_n;&c_je;&c_je; &c_v;&c_r;&c_a;&c_g;&c_a;</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="serv">servema</ref>,
      <ref cel="indulg">indulga</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_l;&c_u;&c_zh;&c_l;&c_ib;&c_v;&c_y;</trd>
  <trdgrp lng="de">
    <trd>gef&auml;llig</trd>,
    <trd>freundlich</trd>,
    <trd>entgegenkommend</trd>,
    <trd>zuvorkommend</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>kindly</trd>,
    <trd>benevolent</trd>,
    <trd>complaisant</trd>,
    <trd>courteous</trd>,
    <trd>friendly</trd>,
    <trd>good natured</trd>,
    <trd>obliging</trd>,
    <trd>willing</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>obligeant</trd>,
    <trd>complaisant<klr tip="ind"> (obligeant)</klr></trd>,
    <trd>serviable</trd>
  </trdgrp>
  <trd lng="nl">welwillend</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_l;&c_u;&c_zh;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_l;&c_ju;&c_b;&c_je;&c_z;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="komple4.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on:
      <ekz>
        &ccirc;u vi <tld/>us traduki al mi tiun vorton?;
      </ekz>
      <ekz>
        <tld/>u al li per via protektado<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la revolucio postulis viktimojn por <tld/>i <klr>(kontentigi)</klr>
        la sangavidon de &gcirc;iaj adeptoj
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vol.bon0i">bonvoli</ref>,
      <ref cel="volont">volonti</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol; &c_l;&c_a;&c_s;&c_k;&c_a;&c_v;&c_y;&c_m;</trd>,
    <trd>&c_z;&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; &c_p;&c_a;&c_s;&c_l;&c_u;&c_g;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gef&auml;llig sein</trd>,
    <trd>freundlich sein</trd>,
    <trd>entgegenkommen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>oblige</trd>,
    <trd>be so <ind>kind</ind> as to</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire le <ind>plaisir</ind> de</trd>,
    <trd>avoir l'<ind>amabilit&eacute;</ind> de</trd>,
    <trd>avoir la <ind>gentillesse</ind> de</trd>,
    <trd>avoir l'<ind>obligeance</ind> de</trd>
  </trdgrp>
  <trd lng="nl">de <ind>goedheid</ind> hebben</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol; &c_u;&c_s;&c_l;&c_u;&c_g;&c_u;</trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol; &c_l;&c_ju;&c_b;&c_je;&c_z;&c_n;&c_y;&c_m;</trd>
  </trdgrp>
</drv>
</art>
