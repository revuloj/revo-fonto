<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kapon</rad>/o
</kap>
<drv mrk="kapon.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Kastrita koko, kiun oni grasigas por &gcirc;in man&gcirc;i.
    </dif>
    <trd lng="de">Kapaun</trd>
    <trdgrp lng="en">
      <trd>capon</trd>,
      <trd>castrated <ind>cock</ind></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>kapoen</trd>,
      <trd>gesneden <ind>haan</ind></trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kastrito:
      <ekz>
        <tld/>vo&ccirc;o.
      </ekz>
    </dif>
    <trd lng="de">Kastrat</trd>
    <trd lng="nl">gecastreerde</trd>
  </snc>
  <trd lng="ru">&c_k;&c_a;&c_p;&c_l;&c_u;&c_n;</trd>
</drv>
</art>
