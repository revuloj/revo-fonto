<art mrk="$Id: kapon.xml,v 1.24 2023/09/01 13:58:55 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kapon</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kapon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kapon.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Kastrita koko, kiun oni grasigas por &gcirc;in man&gcirc;i:
      <ekz>
        vidu, ili e&ccirc; ne vidas nin viroj.
        Terure! Kvaza&ubreve; ni estus <tld/>oj
        <fnt>
          <aut>J. Baghy</aut>,
          <vrk>Sur Sanga Tero</vrk>,
          <lok>1933</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_p;&c_l;&c_u;&c_n;</trd>
    <trdgrp lng="bg">
      <trd>&c_s;&c_k;&c_o;&c_p;&c_je;&c_n; <ind>&c_p;&c_je;&c_t;&c_je;&c_l;</ind></trd>,
      <trd>&c_b;&c_malmol;&c_z;&c_l;&c_mol;&c_o;</trd>
    </trdgrp>
    <trd lng="br">kabon</trd>
    <trd lng="de">Kapaun</trd>
    <trdgrp lng="en">
      <trd>capon</trd>,
      <trd>castrated <ind>cock</ind></trd>
    </trdgrp>
    <trd lng="fr">chapon<klr tip="ind"> (coq ch&acirc;tr&eacute;)</klr></trd>
    <trd lng="hu">kappan</trd>
    <trdgrp lng="nl">
      <trd>kapoen</trd>,
      <trd>gesneden <ind>haan</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="kapon.0o.kastrito">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kastrito:
      <ekz>
        <tld/>vo&ccirc;o.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_k;&c_l;&c_a;&c_d;&c_a;&c_n;&c_je;&c_c;</trd>,
      <trd>&c_l;&c_ja;&c_g;&c_ch;&c_a;&c_n;&c_je;&c_c;</trd>,
      <trd>&c_k;&c_a;&c_s;&c_t;&c_r;&c_a;&c_t;</trd>
    </trdgrp>
    <trd lng="bg">&c_k;&c_a;&c_s;&c_t;&c_r;&c_a;&c_t;</trd>
    <trd lng="br">spazhard</trd>
    <trd lng="de">Kastrat</trd>
    <trdgrp lng="en">
      <trd>eunuch</trd>,
      <trd>castrato</trd>
    </trdgrp>
    <trd lng="fr">castrat</trd>
    <trd lng="hu">kasztr&aacute;lt</trd>
    <trd lng="nl">castraat</trd>
  </snc>
  <trd lng="cs">kapoun</trd>
  <trd lng="es">cap&oacute;n</trd>
  <trdgrp lng="ja">
    <trd>&#x53BB;&#x52E2;&#x304A;&#x3093;&#x3069;&#x308A;  [<ind>&#x304D;&#x3087;&#x305B;&#x3044;&#x304A;&#x3093;&#x3069;&#x308A;</ind>]</trd>,
    <trd>&#x53BB;&#x52E2;&#x3055;&#x308C;&#x305F;&#x4EBA;  [<ind>&#x304D;&#x3087;&#x305B;&#x3044;&#x3055;&#x308C;&#x305F;&#x3058;&#x3093;</ind>]</trd>
  </trdgrp>
  <trd lng="pl">kap&lstroke;on</trd>     
  <trd lng="ro">clapon</trd>
  <trd lng="ru">&c_k;&c_a;&c_p;&c_l;&c_u;&c_n;</trd>
  <trd lng="sk">kap&uacute;n</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_a;&#x43F;&#x43B;&c_u;&#x43D;</trd>,
    <trd>&#x43A;&c_a;&c_s;&c_t;&c_r;&c_a;&c_t;</trd>
  </trdgrp>
</drv>
</art>
