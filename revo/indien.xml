<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>indien</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="indien.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Maldensa <ref tip="vid" cel="koton.0o">kotona</ref>
      kolordesegnita teksa&jcirc;o.
    </dif>
    <ref tip="super" cel="katun.0o">katuno</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_k;&c_a;&c_l;&c_mol;</trd>
  <trdgrp lng="de">
     <trd>Zitz</trd>,
     <trd>feiner buntbedruckter Kattun</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>indienne</trd>,
     <trd>calico</trd>,
     <trd>chintz</trd>
  </trdgrp>
  <trd lng="fr">indienne<klr tip="ind"> (tissu)</klr></trd>
  <trdgrp lng="hu">
     <trd>indiai <ind>karton</ind></trd>,
     <trd>nyomott&aacute;ru</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>sits</trd>,
    <trd>indienne</trd>
  </trdgrp>
  <trd lng="pt">chita<klr tip="amb"> (tecelagem, vestu&aacute;rio)</klr></trd>
  <trd lng="ru">&c_s;&c_i;&c_t;&c_je;&c_c;</trd>
</drv>
</art>
