<kap>
  <rad>indien</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="indien.0o">
  <kap><tld/>o</kap>
  <snc mrk="indien.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Maldensa <ref tip="vid" cel="koton.0o">kotona</ref>
      kolordesegnita teksa&jcirc;o:
      <ekz>
        neniam mi vidis viajn dolarojn <klr>[&#8230;]</klr> ni ricevis rekompencon, kontra&ubreve; &gcirc;i
        ni a&ccirc;etas <tld/>on
        <fnt>
          <bib>MkM</bib>
          <lok>27</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i rapide iris &gcirc;is&scirc;ranken kaj prenis verdan
        silkfulardon <klr>[&#8230;]</klr> el <tld/>o
        <fnt><aut>S. Larbar</aut>, <vrk>Karuseloj</vrk>,
        <lok>1987</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="katun.0o">katuno</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_k;&c_a;&c_l;&c_mol;</trd>
  <trd lng="ca">indiana <klr tip="amb">(teixit)</klr></trd>
  <trdgrp lng="de">
     <trd>Zitz</trd>,
     <trd>feiner buntbedruckter Kattun</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>indienne</trd>,
     <trd>calico</trd>,
     <trd>chintz</trd>
  </trdgrp>
  <trd lng="fr">indienne<klr tip="ind"> (&eacute;toffe)</klr></trd>
  <trdgrp lng="hu">
     <trd>indiai <ind>karton</ind></trd>,
     <trd>nyomott&aacute;ru</trd>
  </trdgrp>
  <trd lng="ja">&#12452;&#12531;&#12489;&#26356;&#32023;  <pr>&#12356;&#12435;&#12393;&#12373;&#12425;&#12373;</pr></trd>
  <trdgrp lng="nl">
    <trd>sits</trd>,
    <trd>indienne</trd>
  </trdgrp>
  <trd lng="pl">indienne</trd>
  <trd lng="pt">chita<klr tip="amb"> (tecelagem, vestu&aacute;rio)</klr></trd>
  <trd lng="ru">&c_s;&c_i;&c_t;&c_je;&c_c;</trd>
  <trd lng="uk">&c_s;&c_i;&c_t;&c_je;&c_c;&c_mol;</trd>
  <trd lng="zh">&#33457;&#24067; <pr>hu&#257;b&ugrave;</pr></trd>
</drv>
</art>
