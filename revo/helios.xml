<art mrk="$Id: helios.xml,v 1.16 2023/08/31 15:46:15 revo Exp $">
<kap>
  <rad>heliostat</rad>/o
</kap>
<drv mrk="helios.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">AST</uzo>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="instru2.0o">Instrumento</ref>, kiu permesas projekcii la
      radiojn de la suno al unu fiksita direkto malgra&ubreve; la movi&gcirc;o de la tero:
      <ekz>
        la plej frua mencio de <tld/>o trovi&gcirc;as en libro de la fizikisto
        <nom>Willem Jacob &#x2019;s Gravesande</nom> el la jaro 1742, la <tld/>o
        rotaciis per horlo&gcirc;a me&hcirc;aniko.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_l;&c_ib;&c_ja;&c_s;&c_t;&c_a;&c_t;</trd>
  <trd lng="de">Heliostat</trd>
  <trd lng="en">heliostat</trd>
  <trd lng="fr">h&eacute;liostat</trd>
  <trdgrp lng="hu">
    <trd>nap&aacute;ll&iacute;t&oacute; (k&eacute;sz&uuml;l&eacute;k)
    </trd>,
    <trd>helioszt&aacute;t</trd>
  </trdgrp>
  <trd lng="it">eliostato</trd>
  <trd lng="ja">&#x30D8;&#x30EA;&#x30AA;&#x30B9;&#x30BF;&#x30C3;&#x30C8;</trd>
  <trd lng="nl">heliostaat</trd>
  <trd lng="pl">heliostat</trd>
  <trd lng="pt">heli&oacute;stato</trd>
  <trd lng="ru">&c_g;&c_je;&c_l;&c_i;&c_o;&c_s;&c_t;&c_a;&c_t;</trd>
  <trd lng="uk">&#x433;&#x435;&#x43B;&#x456;&#x43E;&#x441;&#x442;&#x430;&#x442;</trd>
</drv>
</art>
