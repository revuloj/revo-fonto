<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ne&ubreve;rologi</rad>/o
</kap>
<drv mrk="neuxro.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SCI</uzo>
  <snc>
    <dif>
      Scienco pri la nervoj kaj ilia funkciado.
    </dif>
  </snc>
  <trd lng="de">Neurologie</trd>
  <trd lng="en">neurology</trd>
  <trdgrp lng="fr">
    <trd>neurologie</trd>
  </trdgrp>
  <trd lng="nl">neurologie</trd>
  <trd lng="ru">&c_n;&c_je;&c_v;&c_r;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
</drv>

<drv mrk="neuxro.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
       Fakulo pri <ref tip="vid" cel="neuxro.0o"><tld/>o</ref>
       a&ubreve; <ref tip="super" cel="kurac.0isto">kuracisto</ref>
       pri nervoj kaj <ref tip="vid" cel="neuxro1.0o">ne&ubreve;rozoj</ref>.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="neuxrolo.0o">ne&ubreve;rologo</ref>
  </refgrp>
  <trdgrp lng="de">
    <trd>Neurologe</trd>,
    <trd>Nervenarzt</trd>
  </trdgrp>
  <trd lng="en">neurologist</trd>
  <trdgrp lng="fr">
    <trd>neurologue</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>neuroloog</trd>,
    <trd>zenuwspecialist</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_je;&c_v;&c_r;&c_o;&c_l;&c_o;&c_g;</trd>
</drv>
</art>
