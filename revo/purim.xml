<kap>
  <rad>Purim</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="purim.0o">
  <kap><tld/>o</kap>
  <snc mrk="purim.0o.JUD">
    <uzo tip="fak">KAL</uzo>
    <uzo tip="fak">REL</uzo>
    <dif>
      Unutaga judisma vintra <ref tip="lst" cel="fest.0o" lst="voko:festoj">festo</ref> memoriganta
      la  
      savi&gcirc;on de la hebreoj el pereo en jaro 355 a.K., 
      la&ubreve; rakonto de la biblia libro de Ester:
      <ekz>
        en <tld/>o, la judoj man&gcirc;as specialaj<klr>n</klr> 
        biskvitoj<klr>n</klr> <ctl>Oreloj de Hamano</ctl>
        <fnt>
          <aut>Juda Esperanto Asocio</aut>,
          <vrk><url ref="http://judaesperantoasocio.wordpress.com/">[retejo]</url></vrk>,
          <lok>2009-03-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_P;&c_u;&c_r;&c_y;&c_m;</trd>
  <trd lng="ca">purim</trd>
  <trdgrp lng="cs">
    <trd>Purim</trd>,
    <trd>purim</trd>
  </trdgrp>
  <trd lng="de">Purim</trd>
  <trd lng="fr">Pourim</trd>
  <trd lng="hu">purim</trd>
  <trd lng="nl">purim</trd>
  <trdgrp lng="pl">
    <trd>Purim</trd>,
    <trd>&Sacute;wi&eogonek;to Los&oacute;w</trd>
  </trdgrp>
  <trd lng="ro">Purim</trd>
  <trd lng="sk">Pur&iacute;m</trd>
  <trd lng="uk">&#x41F;&c_u;&c_r;&c_i;&#x43C; (&c_jeu;&c_v;&c_r;&c_je;&c_j;&c_s;&#x44C;&#x43A;&c_je; &c_s;&c_v;&#x44F;&c_t;&#x43E;)</trd>
</drv>
