<art mrk="$Id$">
<kap>
  <rad>tatam</rad>/o <fnt><bib>JapEnc</bib></fnt>
</kap>
<drv mrk="tatam.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Mato proksimume 6 cm dika, 90 cm lar&gcirc;a, 180 cm longa,
