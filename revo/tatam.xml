      apartamentoj.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Tatami</trd>,
     <trd>Judomatte</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>tatami</trd>
  </trdgrp>
