<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: spes.xml,v 1.18 2022/03/29 14:19:45 revo Exp $">
<kap>
  <rad>spes</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="spes.0o">
  <kap><tld/>o</kap>
  <snc mrk="spes.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Fikcia ne&ubreve;trala <ref tip="lst" cel="unu.mon0o" lst="voko:monunuoj">monunuo</ref>:
      <ekz>
        &leftquot;ne&rightquot;, diris la sor&ccirc;istino, &leftquot;e&ccirc; unu <tld/>on mi ne volas
        havi&rightquot;
        <fnt><bib>Fab1</bib>, <lok>fajrilo</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i ne sciis ankora&ubreve; precize, &ccirc;u &scirc;i povos kune kun sia infano
        vivi
        per dudek <tld/>dekoj en tago
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">spes</trd>
  <trd lng="de">Spes</trd>
  <trd lng="en">speso</trd>
  <trdgrp lng="es">
    <trd>unidades monetarias</trd>,
    <trd>u.m.</trd>
  </trdgrp>
  <trd lng="fr">speso</trd>
  <trd lng="nl">speso</trd>
  <trd lng="sk">spes <klr>(medzin&aacute;rodn&aacute; menov&aacute; jednotka ktor&uacute; navrhli
  esperantisti)</klr></trd>
  <trd lng="uk">&#x441;&#x43F;&#x435;&#x441;&#x43E;</trd>
</drv>

<drv mrk="spes.0milo">
  <kap><tld/>milo</kap>
  <snc mrk="spes.0milo.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Mil <tld/>oj.
      <ekz>
        la ringo de via mo&scirc;to valoras tri kaj duonon da spesmiloj
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">tis&iacute;c speso</trd>
  <trd lng="de">Spesmilo</trd>
  <trd lng="en">spesmilo</trd>
  <trd lng="es">miles de unidades monetarias</trd>
  <trd lng="fr">spesmilo</trd>
  <trd lng="sk">tis&iacute;c speso</trd>
</drv></art>
