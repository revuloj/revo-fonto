<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>spes</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="spes.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Internacia fiktiva monunuo proponita de la esperantistoj:
      <ekz>
        <tld/>deko<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kiu <tld/>on ne tenas, al <tld/>milo ne venas<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="pfenig.0o">pfenigo</ref>,
      <ref cel="centim.0o">centimo</ref>,
      <ref cel="grosx.0o">gro&scirc;o</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Spes</trd>
</drv>
</art>
