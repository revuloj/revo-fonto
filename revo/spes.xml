<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: spes.xml,v 1.21 2024/12/09 08:38:22 revo Exp $">
<kap>
  <rad>spes</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="spes.0o">
  <kap><tld/>o</kap>
  <snc mrk="spes.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Fikcia ne&ubreve;trala <ref tip="lst" cel="unu.mon0o" lst="voko:monunuoj">monunuo</ref>:
      <ekz>
        &leftquot;ne&rightquot;, diris la sor&ccirc;istino, &leftquot;e&ccirc; unu <tld/>on mi ne volas
        havi&rightquot;
        <fnt><bib>Fab1</bib>, <lok>fajrilo</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i ne sciis ankora&ubreve; precize, &ccirc;u &scirc;i povos kune kun sia infano
        vivi
        per dudek <tld/>dekoj en tago
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_e;&c_s;&c_a; <klr>(&c_u;&c_ja;&c_w;&c_n;&c_a;&c_ja; &c_g;&c_r;&c_a;&c_sh;&c_o;&c_v;&c_a;&c_ja; &c_a;&c_d;&c_z;&c_ib;&c_n;&c_k;&c_a;)</klr></trd>
  <trd lng="cs">spes</trd>
  <trd lng="de">Spes</trd>
  <trd lng="en">speso</trd>
  <trdgrp lng="es">
    <trd>unidades monetarias</trd>,
    <trd>u.m.</trd>
  </trdgrp>
  <trd lng="fr">speso</trd>
  <trd lng="ja">&#x30B9;&#x30DA;&#x30FC;&#x30BD;</trd>
  <trd lng="nl">speso</trd>
  <trd lng="pl">spes</trd>
  <trd lng="sk">spes <klr>(medzin&aacute;rodn&aacute; menov&aacute; jednotka ktor&uacute; navrhli
  esperantisti)</klr></trd>
  <trd lng="uk">&c_s;&#x43F;&c_je;&c_s;&#x43E;</trd>
</drv>

<drv mrk="spes.0milo">
  <kap><tld/>milo</kap>
  <snc mrk="spes.0milo.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Mil <tld/>oj.
      <ekz>
        la ringo de via mo&scirc;to valoras tri kaj duonon da spesmiloj
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_y;&c_s;&c_ja;&c_ch;&c_a; <ind>&c_s;&c_p;&c_e;&c_s;&c_a;&c_w;</ind></trd>
  <trd lng="cs">tis&iacute;c speso</trd>
  <trd lng="de">Spesmilo</trd>
  <trd lng="en">spesmilo</trd>
  <trd lng="es">miles de unidades monetarias</trd>
  <trd lng="fr">spesmilo</trd>
  <trd lng="ja">&#x30B9;&#x30DA;&#x30B9;&#x30DF;&#x30FC;&#x30ED;</trd>
  <trd lng="pl">tysi&aogonek;c  <ind>spes&oacute;w</ind></trd>
  <trd lng="sk">tis&iacute;c speso</trd>
</drv></art>
