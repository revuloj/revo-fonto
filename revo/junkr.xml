<vortaro>
<art mrk="$Id$">
<kap>
  <rad>junkr</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="junkr.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Juna <ref tip="super" cel="nobel.0o.POL">nobelo</ref>:
      <ekz>
        ni estas ses servistoj por la sinjoro, la sinjorino,
        la <tld/>o kaj la du fra&ubreve;linoj
        <fnt>
          <aut>S. S. Blicher, trad. H. J. Bulthuis</aut>,
          <vrk>Taglibro de Vila&gcirc;-pedelo</vrk>,
          <lok>1922</lok>
        </fnt>.
      </ekz>        
    </dif>
  </snc>
  <trd lng="be">&c_ju;&c_n;&c_k;&c_je;&c_r;</trd>
  <trd lng="de">Junker</trd>
  <trd lng="fr">junker</trd>
  <trd lng="hu">junker</trd>
  <trdgrp lng="it">
    <trd>junker</trd>, 
    <trd>signorotto</trd>
  </trdgrp>
  <trd lng="nl">jonker</trd>
  <trd lng="pt">junker</trd>
  <trd lng="ru">&c_ju;&c_n;&c_k;&c_je;&c_r;</trd>
</drv>
</art>
