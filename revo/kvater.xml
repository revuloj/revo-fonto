      <ref cel="primar.0a">primara</ref>,
      <ref cel="sekund2.0a">sekundara</ref>,
      <ref cel="tercia.0a">terciara</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">quartair</trd>
  <trd lng="ru">&c_ch;&c_je;&c_t;&c_v;&c_je;&c_r;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
