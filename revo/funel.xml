<drv mrk="funel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Ilo, kies supra parto estas konusforma, k fini&gcirc;anta per rekta
      tubo,
      uzata por ver&scirc;i flua&jcirc;on en barelon, vazon, botelon kc.  
    </dif>
  </snc>
  <trd lng="de">Trichter</trd>
  <trd lng="en">funnel</trd>
  <trd lng="es">embudo</trd>
  <trd lng="fr">entonnoir</trd>
  <trd lng="nl">trechter</trd>
  <trd lng="pt">funil</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_r;&c_o;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_t;&c_r;&c_u;&c_b;</trd>
  </trdgrp>
</drv>
</art>
