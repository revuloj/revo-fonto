<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sukot.xml,v 1.5 2024/03/25 07:28:37 revo Exp $">
<kap>
  <rad>Sukot</rad>/o
</kap>
<drv mrk="sukot.0o">
  <kap><tld/>o</kap>
  <snc mrk="sukot.0o.JUD">
    <uzo tip="fak">REL</uzo>
    <dif>
      <ref tip="dif" cel="fest.0odelauxboj.JUD">Festo de La&ubreve;boj</ref>:
      <ekz>
        la festo nomata <tld/>o <klr>[&#x2026;]</klr> rememor<klr>ig</klr>is la 
        liberigon de la egipta sklaveco
        <fnt>
          <aut>P. A. Maggi</aut>,
          <vrk><url ref="http://www.studibiblici.it/ESPERANTO/Omelie/28%2002%2010%20rev.pdf">
          Dua Karesma Diman&ccirc;o</url></vrk>,
          <lok>2010-02-28</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum bibliaj tempoj <tld/>o estis la festo, kiam la nacioj de la 
        mondo venis al Jerusalemo por celebri kun la juda popolo
        <fnt>
          <aut>Rita</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/ritafoto/311553">
          Sukoto</url></vrk>,
          <lok>2011-02-25</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_S;&c_u;&c_k;&c_o;&c_t;</trd>,
    <trd>&c_S;&c_mol;&c_v;&c_ja;&c_t;&c_a; &c_k;&c_u;&c_sh;&c_ch;&c_a;&c_w;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Sukkot</trd>,
    <trd>Laubh&uuml;ttenfest</trd>
  </trdgrp>
  <trd lng="fr">Souccot</trd>
  <trdgrp lng="pl">
    <trd>Sukkot</trd>,
    <trd>Kuczki</trd>,
    <trd>&Sacute;wi&eogonek;to <ind>Namiot&oacute;w</ind></trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4F4F;&#x68DA;&#x7BC0; <pr>zh&#xF9;p&#xE9;ngji&#xE9;</pr></trd>,
    <trd>&#x4F4F;&#x68DA;&#x8282; <pr>zh&#xF9;p&#xE9;ngji&#xE9;</pr></trd>
  </trdgrp>
</drv>
</art>

