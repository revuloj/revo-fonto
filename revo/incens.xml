<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>incens</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="incens.0o">
  <kap><tld/>o</kap>
  <snc mrk="incens.0o.rezino">
    <dif>
      Nomo de kelkaj specoj de rezinoj, brulantaj kun bonodora fumo
      kaj uzataj en iuj religiaj ceremonioj:
      <ekz>
        la <tld/>fumo flugis supren al la kupolo
        <fnt><aut>J. Baghy</aut>, <vrk>Sur Sanga Tero</vrk>, 
        <lok>1933</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="odor.bon0fumajxo">bonodorfuma&jcirc;o</ref>;
    </refgrp>
    <refgrp tip="vid">
      <ref cel="oliban.0o">olibano</ref>,
      <ref cel="parfum.0o">parfumo</ref>
    </refgrp>
    <adm>
      Vd adm. sub "bonodorfuma&jcirc;o". <aut>MB</aut>
    </adm>
    <trdgrp lng="de">
       <trd>Weihrauch</trd>,
       <trd>R&auml;ucherwerk</trd>
    </trdgrp>
    <trd lng="en">incense</trd>
    <trdgrp lng="fa">
      <trd>&a_b;&a_kh;&a_w;&a_r;</trd>,
      <trd>&f_k;&a_n;&a_d;&a_r;</trd>
    </trdgrp>
    <trd lng="hu">t&ouml;mj&eacute;n</trd>
    <trd lng="pt">incenso</trd>
    <trdgrp lng="ru">
      <trd>&c_l;&c_a;&c_d;&c_a;&c_n;</trd>,
      <trd>&c_f;&c_i;&c_m;&c_i;&c_a;&c_m;</trd>
    </trdgrp>
  </snc>
  <snc mrk="incens.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Agrableco de la&ubreve;doj, de flata&jcirc;oj.
    </dif>
    <trdgrp lng="fa">
      <trd>&f_ch;&a_r;&a_b;&zwnj;&a_z;&a_b;&a_A;&a_n;&f_y;</trd>,
      <trd>&a_t;&a_m;&a_l;&a_q;</trd>
    </trdgrp>
    <trd lng="hu">t&ouml;mj&eacute;nf&uuml;st<klr tip="amb"> (&aacute;tv.)</klr></trd>
  </snc>
  <trd lng="de">Beweihr&auml;ucherung</trd>
  <trd lng="es">incienso</trd>
  <trdgrp lng="fr">
    <trd>encens</trd>,
    <trd>louanges</trd>
  </trdgrp>
  <trd lng="nl">wierook</trd>
  <trd lng="ru">&c_l;&c_je;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="incens.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="incens.0i.per_rezino">
    <dif>
      Honori iun a&ubreve; ion bruligante <tld/>on:
      <ekz>
        <tld/>i pastron, altaron, &ccirc;erkon.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="odor.bon0fumigi">bonodorfumigi</ref>.
    </refgrp>
    <trd lng="de"><klr>mit </klr>Weihrauch <ind>ber&auml;uchern</ind></trd>
    <trdgrp lng="fa">
      <trd>&f_k;&a_n;&a_d;&a_r; &a_s;&a_w;&a_z;&a_A;&a_n;&a_d;&a_n;</trd>,
      <trd>&a_m;&a_j;&a_m;&a_r; &f_g;&a_r;&a_d;&a_A;&a_n;&a_d;&a_n;</trd>
    </trdgrp>
    <trd lng="pt">incensar</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_k;&c_u;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol; &c_f;&c_i;&c_m;&c_i;&c_a;&c_m;&c_o;&c_m;</trd>
    </trdgrp>
  </snc>
  <snc mrk="incens.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      La&ubreve;di, flati:
      <ekz>
        konstanta <tld/>ado<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">beweihr&auml;uchern</trd>
    <trdgrp lng="fa">
      <trd>&f_ch;&a_r;&a_b;&zwnj;&a_z;&a_b;&a_A;&a_n;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>,
      <trd>&a_t;&a_m;&a_l;&a_q; &f_g;&a_f;&a_t;&a_n;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_l;&c_mol;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_k;&c_u;&c_r;&c_i;&c_t;&c_mol; &c_f;&c_i;&c_m;&c_i;&c_a;&c_m; <klr>(&c_k;&c_o;&c_m;&c_u;-&c_l;.)</klr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>encenser</trd>,
    <trd>louanger</trd>
  </trdgrp>
  <trd lng="hu">t&ouml;mj&eacute;nez</trd>
  <trd lng="nl">bewieroken</trd>
</drv>

<drv mrk="incens.0ilo">
  <kap><tld/>ilo<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Speco de balancebla bruligilo por incenso.
    </dif>
  </snc>
  <adm>
    La vorto "bruligilo", aperanta en dif, mankas en
    REVO. Jam ekzistas "brulilo", kiu &scirc;ajne ne konvenas
    (kvankam uzata de PV en &ccirc;i tiu difino "parfumbrulilo").
    <aut>MB</aut>
  </adm>
  <trdgrp lng="de">
     <trd>R&auml;ucherfass</trd>,
     <trd>Weihrauchfass</trd>,
     <trd>Weihrauchgef&auml;&szlig;</trd>
  </trdgrp>
  <trd lng="fa">&a_m;&a_j;&a_m;&a_r;</trd>
  <trd lng="fr">encensoir</trd>
  <trd lng="hu">f&uuml;st&ouml;l&odblac;<klr tip="ind"> (templomi)</klr></trd>
  <trd lng="nl">wierookvat</trd>
  <trd lng="pt">tur&iacute;bulo</trd>
  <trd lng="ru">&c_k;&c_a;&c_d;&c_i;&c_l;&c_o;</trd>
</drv>
</art>
