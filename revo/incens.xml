<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: incens.xml,v 1.24 2021/05/29 14:32:35 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>incens</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="incens.0o">
  <kap><tld/>o</kap>
  <snc mrk="incens.0o.rezino">
    <dif>
      Kelkaj specoj de <ref tip="super" cel="rezin.0o.BOT">rezino</ref>j, brulantaj kun bonodora fumo
      kaj uzataj en iuj religiaj ceremonioj:
      <ekz>
        faru altaron, por fumigi <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 30:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        faru el tio <tld/>a&jcirc;on kunmetitan la&ubreve; la arto de la &scirc;mira&jcirc;isto, bone
        frotmiksitan, puran, sanktan
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 30:35</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia patrino bruligis kandelojn kaj <tld/>on kaj donacis monon al la mona&hcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Xu Jinming/pg</aut>
          <vrk><url ref="https://www.monato.be/2014/011281.php">Religioj por nereligiemuloj</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>fumo flugis supren al la kupolo
        <fnt><aut>J. Baghy</aut>, <vrk>Sur Sanga Tero</vrk>, 
        <lok>1933</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="odor.bon0fumajxo">bonodorfuma&jcirc;o</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="oliban.0o">olibano</ref>,
      <ref cel="parfum.0o">parfumo</ref>
    </refgrp>
    <adm>
      Vd adm. sub "bonodorfuma&jcirc;o". <aut>MB</aut>
    </adm>
    <trd lng="ca">encens</trd>
    <trdgrp lng="de">
       <trd>Weihrauch</trd>,
       <trd>R&auml;ucherwerk</trd>
    </trdgrp>
    <trd lng="en">incense</trd>
    <trdgrp lng="fa">
      <trd>&a_b;&a_kh;&a_w;&a_r;</trd>,
      <trd>&f_k;&a_n;&a_d;&a_r;</trd>
    </trdgrp>
    <trd lng="hu">t&ouml;mj&eacute;n</trd>
    <trd lng="pt">incenso</trd>
    <trdgrp lng="ru">
      <trd>&c_l;&c_a;&c_d;&c_a;&c_n;</trd>,
      <trd>&c_f;&c_i;&c_m;&c_i;&c_a;&c_m;</trd>
    </trdgrp>
  </snc>
<!-- mi ne trovis pruvon de tia uzo!
  <snc mrk="incens.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Agrableco de la&ubreve;doj, de flata&jcirc;oj.
    </dif>
    <trdgrp lng="ca">
      <trd>encomi</trd>,
      <trd>llagot</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&f_ch;&a_r;&a_b;&zwnj;&a_z;&a_b;&a_A;&a_n;&f_y;</trd>,
      <trd>&a_t;&a_m;&a_l;&a_q;</trd>
    </trdgrp>
    <trd lng="hu">t&ouml;mj&eacute;nf&uuml;st<klr tip="amb"> (&aacute;tv.)</klr></trd>
  </snc>
-->
  <trd lng="bo">&#xF66;&#xFA4;&#xF7C;&#xF66;&#xF0B;</trd>
  <trd lng="cs">kou&rcaron; kadidla</trd>
  <trd lng="de">Beweihr&auml;ucherung</trd>
  <trd lng="es">incienso</trd>
  <trdgrp lng="fr">
    <trd>encens</trd>,
    <trd>louanges</trd>
  </trdgrp>
  <trd lng="nl">wierook</trd>
  <trd lng="ru">&c_l;&c_je;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">dym kadidla</trd>
</drv>

<drv mrk="incens.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="incens.0i.per_rezino">
    <dif>
      <ref tip="super" cel="sankt.0igi">Sanktigi</ref> per la fumo de <tld/>o:
      <ekz>
        <tld/>i fajroferon al la Eternulo
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 30:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iama <tld/>ado anta&ubreve; la Eternulo
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 30:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is la statuojn de l' dioj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni <tld/>as tion, kion la popolo devas respekti
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        al mi <klr>[&#x2026;]</klr> oni devus <tld/>i, &ccirc;ar mi donis al li la ideon por lia plej
        glora kanto
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="odor.bon0fumigi">bonodorfumigi</ref>
    </refgrp>
    <trd lng="ca">encensar</trd>
    <trd lng="de"><klr>mit </klr>Weihrauch <ind>ber&auml;uchern</ind></trd>
    <trd lng="es">incensar</trd>
    <trdgrp lng="fa">
      <trd>&f_k;&a_n;&a_d;&a_r; &a_s;&a_w;&a_z;&a_A;&a_n;&a_d;&a_n;</trd>,
      <trd>&a_m;&a_j;&a_m;&a_r; &f_g;&a_r;&a_d;&a_A;&a_n;&a_d;&a_n;</trd>
    </trdgrp>
    <trd lng="pt">incensar</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_k;&c_u;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol; &c_f;&c_i;&c_m;&c_i;&c_a;&c_m;&c_o;&c_m;</trd>
    </trdgrp>
  </snc>
  <snc mrk="incens.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <uzo tip="stl">RAR</uzo>
    <dif>
      La&ubreve;di, flati:
      <ekz>
        la rozokolora dio Eroso <klr>[&#x2026;]</klr> atestu, ke nia tuta vivo estas nenio alia
        ol konstanta <tld/>ado pro lia honoro
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">encomiar</trd>
    <trd lng="de">beweihr&auml;uchern</trd>
    <trdgrp lng="fa">
      <trd>&f_ch;&a_r;&a_b;&zwnj;&a_z;&a_b;&a_A;&a_n;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>,
      <trd>&a_t;&a_m;&a_l;&a_q; &f_g;&a_f;&a_t;&a_n;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_l;&c_mol;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_k;&c_u;&c_r;&c_i;&c_t;&c_mol; &c_f;&c_i;&c_m;&c_i;&c_a;&c_m; <klr>(&c_k;&c_o;&c_m;&c_u;-&c_l;.)</klr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>oku&rcaron;ovat</trd>,
    <trd>p&aacute;lit kadidlo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>encenser</trd>,
    <trd>louanger</trd>
  </trdgrp>
  <trd lng="hu">t&ouml;mj&eacute;nez</trd>
  <trd lng="nl">bewieroken</trd>
  <trdgrp lng="sk">
    <trd>pochlebova&tcaron;</trd>,
    <trd>zalie&ccaron;a&tcaron; sa</trd>
  </trdgrp>
</drv>


<drv mrk="incens.0ujo">
  <kap><tld/>ujo, <var><kap><tld/>ilo</kap></var></kap>
  <snc mrk="incens.0ujo.PAR">
    <dif>
      <ref tip="super" cel="vaz.0o">Vazo</ref>, en kiu oni bruligas <tld/>on kaj
      el kiu disvasti&gcirc;as &gcirc;ia odorfumo; foje kun &ccirc;eno por svingi &gcirc;in:
      <ekz>
        la filoj de <nom>Aaron</nom> prenis &ccirc;iu sian <tld/>ujon kaj metis en &gcirc;in fajron kaj metis
        sur &gcirc;in <tld/>on kaj alportis anta&ubreve; la Eternulon
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 10:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        li prenos plenan <tld/>ujon da ardantaj karboj de la altaro, kiu staras anta&ubreve; la
        Eternulo, kaj plenajn plenmanojn da bonodora pistita <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 16:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        fumstrio levi&gcirc;adis el fera <tld/>ujo
        <fnt>
          <aut>Ba Jin, trad. Li Shijun</aut>,
          <vrk>A&ubreve;tuno en la Printempo</vrk>,
          <lok>1980</lok>
        </fnt>;
      </ekz>
      <ekz>
        an&gcirc;elo venis, kaj staris apud la altaro, havante <tld/>ilon oran
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 8:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        la an&gcirc;elo prenis la <tld/>ilon, kaj li plenigis &gcirc;in per la fajro de la altaro
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 8:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        blanke vestitaj knaboj svingas orajn <tld/>ilojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        belaj ri&ccirc;e vestitaj &hcirc;orknaboj svingis ar&gcirc;entajn <tld/>ilojn
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/e/e3/Thurifer_(4373674893).jpg/296px-Thurifer_(4373674893).jpg" prm="CC BY 2.0"><tld/>anto svinganta <tld/>ilon
        <fnt>
          <aut>Fr James Bradley el Southampton, UK</aut>
          <lok><url ref="&WCW;/File:Thurifer_(4373674893).jpg">https://commons.wikimedia.org/wiki/File:Thurifer_(4373674893).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="ca">encenser</trd>
  <trd lng="cs">kadidelnice</trd>
  <trdgrp lng="de">
     <trd>R&auml;ucherfass</trd>,
     <trd>Weihrauchfass</trd>,
     <trd>Weihrauchgef&auml;&szlig;</trd>
  </trdgrp>
  <trd lng="es">incensario</trd>
  <trd lng="fa">&a_m;&a_j;&a_m;&a_r;</trd>
  <trd lng="fr">brule-parfum</trd>
  <trd lng="hu">f&uuml;st&ouml;l&odblac;<klr tip="ind"> (templomi)</klr></trd>
  <trd lng="nl">wierookvat</trd>
  <trd lng="pt">tur&iacute;bulo</trd>
  <trd lng="ru">&c_k;&c_a;&c_d;&c_i;&c_l;&c_o;</trd>
  <trd lng="sk">kadidelnica</trd>
</drv>
</art>
