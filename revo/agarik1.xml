<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
 
<vortaro>
<art mrk="$Id$">
<kap><rad>agarikac</rad>/oj</kap>
<drv mrk="agarik1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="fung.klab0oj">klabfungoj</ref>
      (<trd lng="la">Agaricaceae</trd>),
      kies ombrelforma fruktokorpo prezentas de sube
      alcentrajn lamenojn.
    </dif>
    <ref tip="prt" cel="fung.lamen0o">lamenfungo</ref>
    <refgrp tip="sub">
      <ref cel="cxampinjon.0o">&ccirc;ampinjono</ref>,
      <ref cel="amanit.0o">amanito</ref>.
    </refgrp>
  </snc>
  <trd lng="fr">agaricac&eacute;es</trd>
  <trd lng="pt">agaric&aacute;ceas</trd>
</drv>
</art>
</vortaro>
