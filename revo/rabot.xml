<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rabot</rad>/i
</kap>
<drv mrk="rabot.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ebenigi a&ubreve; glatigi lignon a&ubreve; metalon, tran&ccirc;ante malebena&jcirc;ojn per
      speciala ilo.
    </dif>
    <refgrp tip="sin">
      <ref cel="fajl.0i">fajli</ref>.
    </refgrp>
  </snc>
  <trd lng="de">hobeln</trd>
  <trd lng="en">plane</trd>
  <trd lng="nl">schaven</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_r;&c_u;&c_g;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_o;&c_g;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="rabot.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Instrumento por <tld/>i.
    </dif>
  </snc>
  <trd lng="de">Hobel</trd>
  <trd lng="nl">schaaf</trd>
  <trd lng="ru">&c_r;&c_u;&c_b;&c_a;&c_n;&c_o;&c_k;</trd>
</drv>

<drv mrk="rabot.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Maldikaj rubandaj pecetoj eltran&ccirc;itaj per <tld/>ado.
    </dif>
  </snc>
  <trd lng="de">Hobeln</trd>
  <trd lng="nl">schaafkrullen</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_u;&c_zh;&c_k;&c_a;</trd>
</drv>
</art>
