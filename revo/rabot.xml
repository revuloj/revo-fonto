<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rabot</rad>/i
</kap>
<drv mrk="rabot.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ebenigi, glatigi kaj maldikigi lignon a&ubreve; metalon, tran&ccirc;ante
malebena&jcirc;ojn de la surfaco per speciala ilo.  
    </dif>
    <refgrp tip="sin">
      <ref cel="fajl.0i">fajli</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_b;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_r;&c_je;&c_zh;&c_a;</trd>
  <trd lng="cs">hoblovat</trd>
  <trd lng="de">hobeln</trd>
  <trdgrp lng="en">
    <trd>shave</trd>,
    <trd>plane</trd>,
    <trd>slice</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>aplanar</trd>,
    <trd>alisar<klr tip="amb"> (madera o metal)</klr></trd>
  </trdgrp>
  <trd lng="fr">raboter</trd>
  <trd lng="hu">gyalul</trd>
  <trd lng="nl">schaven</trd>
  <trdgrp lng="pl">
    <trd>struga&cacute;</trd>,
    <trd>heblowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>aplainar</trd>,
   <trd>lavrar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_r;&c_u;&c_g;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_o;&c_g;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">hobľovať</trd>
  <trd lng="sv">hyvla</trd>
</drv>

<drv mrk="rabot.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Instrumento por <tld/>i. Ekzistas rabotiloj por prilabori permane
      lignon, kiuj estas tran&ccirc;iloj fiksita en speciala aperato por
      protekti la manon de la meblisto kaj difini la dikecon de la
      rabota&jcirc;o. La ma&scirc;inoj por raboti metalon funkcias per iloj, kiuj
      tre similas al tiuj uzataj por torni.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_e;&c_b;&c_je;&c_l;&c_mol;</trd>
  <trd lng="bg">&c_r;&c_je;&c_z;&c_a;&c_ch;&c_k;&c_a;</trd>
  <trd lng="cs">hoblík</trd>
  <trd lng="de">Hobel</trd>
  <trd lng="en">slicer</trd>
  <trd lng="fr">rabot</trd>
  <trd lng="hu">gyalu</trd>
  <trd lng="nl">schaaf</trd>
  <trdgrp lng="pl">
    <trd>strug</trd>,
    <trd>hebel</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_u;&c_b;&c_a;&c_n;&c_o;&c_k;</trd>
  <trd lng="sk">hoblík</trd>
  <trd lng="sv">hyvel</trd>
</drv>

<drv mrk="rabot.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Maldikaj rubandaj pecetoj eltran&ccirc;itaj per <tld/>ado.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_b;&c_l;&c_ju;&c_sh;&c_k;&c_a;</trd>
  <trd lng="bg">&c_t;&c_a;&c_l;&c_a;&c_sh;</trd>
  <trdgrp lng="de">
     <trd>Sp&auml;ne</trd>,
     <trd>Hobelsp&auml;ne</trd>
  </trdgrp>
  <trd lng="en">shavings</trd>
  <trd lng="fr">copeau</trd>
  <trd lng="hu">gyaluforg&aacute;cs</trd>
  <trd lng="nl">schaafkrullen</trd>
  <trdgrp lng="pl">
    <trd>wi&oacute;r</trd>,
    <trd>str&oacute;&zdot;yna</trd>,
    <trd>trocina</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_u;&c_zh;&c_k;&c_a;</trd>
  <trd lng="sv">hyvelsp&aring;n</trd>
</drv>
</art>
