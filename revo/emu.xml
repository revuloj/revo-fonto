<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: emu.xml,v 1.8 2023/11/07 20:34:10 revo Exp $">
<kap>
  <rad>emu</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="emu.0o">
  <kap><tld/>o</kap>
  <snc mrk="emu.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda neflugipova birdo
      vivanta en A&ubreve;stralio;
      sola viva genro de familio dromiceoj
      (<trd lng="la">Dromaius</trd>):
      <ekz>
        iliaj viroj &ccirc;asas kanguruojn kaj <tld/>ojn
        kaj aliajn grandajn bestojn
        <fnt>
          <aut>T. Steele</aut>,
          <vrk>Flugi kun kakatuoj</vrk>,
          <lok>2010</lok>
        </fnt>.        
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_m;&c_u;</trd>
  <trd lng="cs">emu</trd>
  <trd lng="de">Emu</trd>
  <trd lng="en">emu</trd>
  <trd lng="fr">&eacute;meu</trd>
  <trd lng="pl">emu</trd>
  <trd lng="sk">emu</trd>
  <trd lng="uk">&c_je;&#x43C;&c_u;</trd>
  <trd lng="zh">&#x9D2F;&#x9D93;&#x79D1; <pr>&#xE9;rmi&#xE1;ok&#x113;</pr></trd>
</drv>
</art>

