<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Uniks</rad>/o
    </kap>

    <drv mrk="uniks.0o">
      <kap>
        <tld/>o
      </kap>
      <uzo tip="fak">KOMP</uzo>
      <snc>
        <dif>
          Historie kaj komerce grava
          <ref tip="super" cel="operac.0umo">operaciumo</ref>,
          disponebla por diversaj komputiloj.
        </dif>
      </snc>
      <trd lng="en">Unix</trd>
      <trd lng="ru">&c_Ju;&c_n;&c_i;&c_k;&c_s;</trd>
    </drv>

    <drv mrk="uniks.0ulo">
      <kap>
        <tld lit="u"/>ulo
      </kap>
      <uzo tip="fak">KOMP</uzo>
      <snc>
        <dif>
          Uzanto (kaj kutime, anka&ubreve; adepto) de <tld/>o:
          <ekz>
            tiu vorto &leftquot;<tld lit="u"/>ulo&rightquot; ta&ubreve;gas, &ccirc;ar <tld/>o,
            kiel aliaj mastrumaj sistemoj, havas sian propran kulturon
            <fnt>
              <aut>E. Grimley Evans</aut>:
              <vrk>Uniksulo ekspertas Unikodon</vrk>.
              <lok><url
                  ref="http://www.esperanto.be/fel/mon/006801.html">Monato</url></lok>.
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="en">
        <trd>Unix user</trd>,
        <trd>Unix fan</trd>
      </trdgrp>
      <trd lng="ru">&c_p;&c_o;&c_l;&c_mol;&c_z;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol; &c_Ju;&c_n;&c_i;&c_k;&c_s;&c_a;</trd>
    </drv>

  </art>

  <!--
  $Log$
  Revision 1.1  2002/01/07 17:32:12  revo
  Sergio Pokrovskij: nova artikolo

  -->
</vortaro>
