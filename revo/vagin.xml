<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>9</ofc><rad>vagin</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="vagin.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      La kanalo de hominoj kaj multaj mamulinoj inter la <ref
      cel="vulv.0o">vulvo</ref> kaj la <ref
      cel="uter.0kolo">uterkolo</ref> uzata por <ref
      cel="kun.seks0igxo">sekskuni&gcirc;o</ref> kaj <ref
      cel="nask.0o">nasko</ref>.
    </dif>
    <rim>
      Oni ne konfuzu la vortojn <tld/>o kaj <ref
      cel="vulv.0o">vulvo</ref>. Anatomie la vagino estas interna kaj la
      vulvo estas ekstera. Nomi la vulvon <tld/>o analogas al nomi la
      lipojn bu&scirc;o a&ubreve; gor&gcirc;o.
    </rim>
  </snc>
  <trd lng="af">vagina</trd>
  <trdgrp lng="be">
    <trd>&c_p;&c_o;&c_h;&c_v;&c_a;</trd>,
    <trd>&c_v;&c_a;&c_g;&c_ib;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_v;&c_l;&c_a;&c_g;&c_a;&c_l;&c_i;&c_shch;&c_je;</trd>
  <trd lng="ca">vagina</trd>
  <trd lng="cs">pochva</trd>
  <trdgrp lng="de">
    <trd>Vagina</trd>,
    <trd>Scheide</trd>
  </trdgrp>
  <trdgrp lng="el">
    <trd>&kappa;&omikron_ton;&lambda;&pi;&omikron;&sigma_fina;</trd>,
    <trd>&kappa;&omikron;&lambda;&epsilon;&omikron_ton;&sigma_fina;</trd>
  </trdgrp>
  <trd lng="en">vagina</trd>
  <trd lng="es">vagina</trd>
  <trd lng="fa">&a_m;&a_h;&a_b;&a_l;</trd>
  <trdgrp lng="fi">
    <trd>em&auml;tin</trd>,
    <trd>vagina</trd>
  </trdgrp>
  <trd lng="fr">vagin</trd>
  <trd lng="fy">fagina</trd>
  <trdgrp lng="he">
    <trd>&nun;&resh;&tav;&yod;&qof;<klr tip="ind"> (&bet;&alef;&nun;&tet;&vav;&mem;&yod;&he;)</klr></trd>,
    <trd>&vav;&gimel;&yod;&nun;&he;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>h&uuml;vely</trd>,
    <trd>vagina</trd>
  </trdgrp>
  <trd lng="id">vagina</trd>
  <trd lng="io">vagino</trd>
  <trd lng="it">vagina</trd>
  <trd lng="iu">&#5234;&#5469;&#5461;&#5251;</trd>
  <trd lng="ja">&#33187; [<ind>&#12385;&#12388;</ind>]</trd>
  <trd lng="jbo">vibna</trd>
  <trdgrp lng="ka">
    <trd>&#4309;&#4304;&#4306;&#4312;&#4316;&#4304;</trd>,
    <trd>&#4321;&#4304;&#4328;&#4317;</trd>
  </trdgrp>
  <trd lng="nl">vagina</trd>
  <trd lng="pl">pochwa<klr> (anatomia)</klr></trd>
  <trd lng="pt">vagina</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_l;&c_a;&c_g;&c_a;&c_l;&c_i;&c_shch;&c_je;</trd>,
    <trd>&c_v;&c_a;&c_g;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="sv">vagina</trd>
  <trd lng="tp">lupa meli</trd>
  <trd lng="tr">vajina</trd>
  <trd lng="vo">vagin</trd>
</drv>

<drv mrk="vagin.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Rilata al <tld/>o.
    </dif>
  </snc>
  <trd lng="cs">po&scaron;evn&iacute;</trd>
  <trd lng="de">vaginal</trd>
  <trd lng="en">vaginal</trd>
  <trd lng="es">vaginal</trd>
  <trdgrp lng="fi">
    <trd>em&auml;tin-</trd>,
    <trd>vaginaalinen</trd>
  </trdgrp>
  <trd lng="fr">vaginal</trd>
  <trd lng="fy">faginaal</trd>
  <trd lng="io">vaginala</trd>
  <trdgrp lng="is">
    <trd>legganga-</trd>,
    <trd>skei&#240;ar-</trd>,
    <trd>sl&iacute;&#240;ur-</trd>
  </trdgrp>
  <trd lng="it">vaginale</trd>
  <trd lng="ja">&#33187;&#12398; [<ind>&#12385;&#12388;&#12398;</ind>]</trd>
  <trd lng="nl">vaginaal</trd>
  <trd lng="pl">pochwowy</trd>
  <trd lng="pt">vaginal</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_a;&c_g;&c_i;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_l;&c_a;&c_g;&c_a;&c_l;&c_i;&c_shch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">vaginal</trd>
  <trd lng="tl">ng kaluban</trd>
  <trd lng="tr">vajinal</trd>
  <trd lng="vo">vaginik</trd>
</drv>
</art>
