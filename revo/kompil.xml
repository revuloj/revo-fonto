<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>1</ofc> <rad>kompil</rad>/i</kap>
<drv mrk="kompil.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="verk.0i">Verki</ref> ne originale, kunigante en unu aron tekstojn pri komuna
      temo, kolektitajn el diversaj fontoj:
      <ekz>
        <tld/>i dokumentojn, libron.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>zusammenstellen</trd>,
     <trd>zusammentragen</trd>,
     <trd>kompilieren</trd>
  </trdgrp>
  <trd lng="en">compile</trd>
  <trd lng="fr">compiler</trd>
  <trd lng="nl">samenstellen</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_p;&c_i;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_o;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="kompil.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        <tld/>o donas iafoje verkon pli bonan ol la originalo<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Zusammenstellen</trd>,
     <trd>Zusammenstellung</trd>,
     <trd>Zusammentragen</trd>,
     <trd>Kompilation</trd>,
     <trd>Kompilieren</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>compilation<klr> (action de compiler)</klr></trd>
  </trdgrp>
  <trd lng="nl">samenstellen</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_i;&c_l;&c_ja;&c_c;&c_i;&c_ja; <klr>(&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;)</klr></trd>
</drv>

<drv mrk="kompil.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      <tld lit="K"/>ita <ref tip="super" cel="verk.0o.propra">verko</ref>.
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="epitom.0o">epitomo</ref>,
    <ref cel="kompen.0o">kompendio</ref>
  </refgrp>
  <trd lng="en">compilation<klr> (ouvrage, r&eacute;sultat d'une compilation)</klr></trd>
  <trdgrp lng="de">
     <trd>Zusammenstellung</trd>,
     <trd>Zusammenfassung</trd>,
     <trd>Kompilation</trd>,
     <trd>Kompilat</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>compilation</trd>
  </trdgrp>
  <trd lng="nl">compilatie</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_i;&c_l;&c_ja;&c_c;&c_i;&c_ja; <klr>(&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je;,
  &c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_t; &c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_ja;)</klr></trd>
</drv>
</art>
