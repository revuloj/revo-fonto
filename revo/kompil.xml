<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kompil.xml,v 1.21 2019/04/24 23:10:17 revo Exp $">
<kap>
  <ofc>1</ofc> <rad>kompil</rad>/i
</kap>
<drv mrk="kompil.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kompil.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="verk.0i">Verki</ref> ne originale, kunigante
      en unu aron tekstojn pri komuna temo, kolektitajn el diversaj
      fontoj:
      <ekz>
        <tld/>i dokumentojn, libron.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_m;&c_p;&c_ib;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">compilar</trd>
  <trdgrp lng="cs">
    <trd>kompilovat</trd>,
    <trd>sestavit <klr>(z r&uring;z. pramen&uring;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>zusammenstellen</trd>,
    <trd>zusammentragen</trd>,
    <trd>kompilieren</trd>
  </trdgrp>
  <trd lng="en">compile</trd>
  <trd lng="es">compilar</trd>
  <trd lng="fr">compiler</trd>
  <trdgrp lng="nl">
    <trd>samenstellen</trd>,
    <trd>compileren</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kompilowa&cacute;</trd>,
    <trd>zbiera&cacute;</trd>,
    <trd>zestawia&cacute;</trd>
  </trdgrp>   
  <trdgrp lng="pt">
    <trd>compilar</trd>,
    <trd>coligir</trd>,
    <trd>reunir</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>compila</trd>,
    <trd>aduna</trd>,
    <trd>potrivi</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_p;&c_i;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_o;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kompilova&tcaron;</trd>,
    <trd>zostavova&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="kompil.0o">
  <kap><tld/>o</kap>
  <snc mrk="kompil.0o.KOMUNE">
    <dif>
      Ago <tld/>i:
      <ekz>
        <tld/>o donas iafoje verkon pli bonan ol la originalo
        <fnt><bib>VdE</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_m;&c_p;&c_ib;&c_l;&c_ja;&c_c;&c_y;&c_ja; <klr>(&c_d;&c_z;&c_je;&c_ja;&c_n;&c_mol;&c_n;&c_je;)</klr></trd>,
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="ca">compilaci&oacute;</trd>
  <trdgrp lng="cs">
    <trd>kompilov&aacute;n&iacute;</trd>,
    <trd>sestavov&aacute;n&iacute;</trd>,
    <trd>vyb&iacute;r&aacute;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Zusammenstellen</trd>,
    <trd>Zusammenstellung</trd>,
    <trd>Zusammentragen</trd>,
    <trd>Kompilation</trd>,
    <trd>Kompilieren</trd>
  </trdgrp>
  <trd lng="es">compilaci&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>compilation<klr tip="ind"> (action)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>compilatie<klr tip="ind"> (het compileren)</klr></trd>,
    <trd>samenstellen<klr tip="ind"> (zn.)</klr></trd>
  </trdgrp>
  <trd lng="pl">kompilacja</trd>     
  <trd lng="ro">compila&#539;ie</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_i;&c_l;&c_ja;&c_c;&c_i;&c_ja; <klr>(&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;)</klr></trd>
  <trdgrp lng="sk">
    <trd>vyberanie</trd>,
    <trd>zostavovanie</trd>
  </trdgrp>
</drv>

<drv mrk="kompil.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="kompil.0ajxo.KOMUNE">
    <dif>
      <tld lit="K"/>ita <ref tip="super"
      cel="verk.0o.propra">verko</ref>.
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="epitom.0o">epitomo</ref>,
    <ref cel="kompen.0o">kompendio</ref>
  </refgrp>
  <trd lng="be">&c_k;&c_a;&c_m;&c_p;&c_ib;&c_l;&c_ja;&c_c;&c_y;&c_ja; <klr>(&c_v;&c_y;&c_n;&c_ib;&c_k;
  &c_p;&c_r;&c_a;&c_c;&c_y;)</klr></trd>
  <trdgrp lng="ca">
    <trd>compilaci&oacute;</trd>,
    <trd>compendi</trd>
  </trdgrp>
  <trd lng="en">compilation</trd>
  <trd lng="es">compilaci&oacute;n</trd>
  <trdgrp lng="de">
    <trd>Zusammenstellung</trd>,
    <trd>Zusammenfassung</trd>,
    <trd>Kompilation</trd>,
    <trd>Kompilat</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>anthologie<klr tip="ind"> (recueil de morceaux
    choisis)</klr></trd>,
    <trd>compilation <klr tip="ind">(ouvrage)</klr></trd>,
    <trd>choix<klr tip="ind"> (recueil de morceaux choisis)</klr></trd>,
    <trd>floril&egrave;ge<klr tip="ind"> (recueil de morceaux
    choisis)</klr></trd>,
    <trd>recueil<klr tip="ind"> (de morceaux choisis)</klr></trd>,
    <trd>s&eacute;lection<klr tip="ind"> (recueil de morceaux
    choisis)</klr></trd>
  </trdgrp>
  <trd lng="nl">compilatie<klr tip="ind"> (resultaat)</klr></trd>
  <trd lng="pl">kompilacja</trd>     
  <trd lng="ro">generare</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_i;&c_l;&c_ja;&c_c;&c_i;&c_ja; <klr>(&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je;,
  &c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_t; &c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_ja;)</klr></trd>
</drv>
</art>
