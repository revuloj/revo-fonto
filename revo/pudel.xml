</kap>
<drv mrk="pudel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Speco de
      <ref tip="super" cel="hund.0o.dombesto">hundo</ref>
      karakterizata per longaj frizitaj haroj.
    </dif>
  </snc>
  <trd lng="de">Pudel</trd>
  <trd lng="nl">poedel</trd>
  <trd lng="fr">caniche</trd>
  <trd lng="ru">&c_p;&c_u;&c_d;&c_je;&c_l;&c_mol;</trd>
</drv>
</art>
