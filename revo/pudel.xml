<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pudel</rad>/o
</kap>
<drv mrk="pudel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Speco de
      <ref tip="super" cel="hund.0o.dombesto">hundo</ref>
      karakterizata per longaj frizitaj haroj.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_d;&c_z;&c_je;&c_l;&c_mol;</trd>
  <trd lng="ca">caniche</trd>
  <trdgrp lng="cs">
    <trd>pudl</trd>,
    <trd>pudlík</trd>
  </trdgrp>
  <trd lng="de">Pudel</trd>
  <trd lng="en">poodle</trd>
  <trd lng="fr">caniche</trd>
  <trdgrp lng="hu">
    <trd>uszk&aacute;r</trd>,
    <trd>pudli</trd>
  </trdgrp>
  <trd lng="nl">poedel</trd>
  <trd lng="pl">pudel</trd>
  <trdgrp lng="pt">
    <trd>poodle</trd>,
    <trd>c&atilde;o-d'&aacute;gua</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_u;&c_d;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">pudlík</trd>
</drv>
</art>
