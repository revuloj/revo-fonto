<drv mrk="ekvili.mal0igi">
  <kap>mal<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Detrui <tld/>on de io:
      <ekz>
        la animo pelas vin super a&ubreve; sub aliajn, <klr>[&#x2026;]</klr> kiuj vin
        mal<tld/>igas
        <fnt>
          <bib>VaK</bib>
