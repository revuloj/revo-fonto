<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>ral</rad>/o
</kap>
<drv mrk="ral.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="ral.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Rallus</trd>).
    </dif>
  </snc>
  <trd lng="cs">ch&rcaron;&aacute;stal vodni</trd>
  <trd lng="de">Wasserralle</trd>
  <trd lng="en">water-rail</trd>
  <trd lng="hu">guvat</trd>
  <trd lng="pl">wodnik</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_u;&c_sh;&c_o;&c_k;</trd>
</drv>
<drv mrk="ral.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="ral.0oformaj"><tld/>oformaj
      birdoj</ref> (<trd lng="la">Rallidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Rallen</trd>
</drv>
<drv mrk="ral.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Ralliformes</trd>).
    </dif>
  </snc>
  <trd lng="de">Rallenv&ouml;gel</trd>
</drv>
</art>
