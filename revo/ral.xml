<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ral</rad>/o
</kap>
<drv mrk="ral.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="raled.0oj"><tld/>edoj</ref>
      (<trd lng="la">Rallus</trd>).
    </dif>
  </snc>
  <trd lng="cs">ch&rcaron;&aacute;stal vodni</trd>
  <trd lng="de">Wasserralle</trd>
  <trd lng="en">water-rail</trd>
  <trd lng="fr">r&acirc;le</trd>
  <trd lng="nl">ral</trd>
  <trd lng="hu">guvat</trd>
  <trd lng="pl">wodnik</trd>
  <trd lng="pt">ralo</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_u;&c_sh;&c_o;&c_k; <klr
tip="amb">(&c_p;&c_t;&c_i;&c_c;&c_a;)</klr></trd>
</drv>

<drv mrk="ral.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Ralliformes</trd>).
    </dif>
  </snc>
  <trd lng="de">Rallenv&ouml;gel</trd>
  <trd lng="fr">gruiformes</trd>
  <trd lng="hu">guvatalak&uacute;ak</trd>
</drv>
</art>
