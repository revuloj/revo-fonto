<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>ral</rad>/o
</kap>
<drv mrk="ral.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="ral.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Rallus</trd>).
    </dif>
  </snc>
  <trd lng="de">Wasserralle</trd>
</drv>
<drv mrk="ral.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="ral.0oformaj"><tld/>oformaj
      birdoj</ref> (<trd lng="la">Rallidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Rallen</trd>
</drv>
<drv mrk="ral.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Ralliformes</trd>).
    </dif>
  </snc>
  <trd lng="de">Rallenv&ouml;gel</trd>
</drv>
</art>

