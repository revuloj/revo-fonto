<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ral.xml,v 1.20 2022/03/26 18:10:07 revo Exp $">
<kap>
  <rad>ral</rad>/o
</kap>

<drv mrk="ral.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el la familio <ref tip="malprt" cel="raled.0oj"><tld/>edoj</ref>
      (<trd lng="la">Rallus</trd>):
      <ekz>
        Virginia <tld/>o estas eta akvobirdo <klr>[kaj]</klr> diferenci&gcirc;as
        disde akvo<tld/>o <klr>[per]</klr> avel- a&ubreve; oran&gcirc;okoloro en brusto kaj kruroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=130673">Virginia ralo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_d;&c_a;&c_k;&c_u;&c_r;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>ch&rcaron;&aacute;stal vodni</trd>,
    <trd>ch&rcaron;&aacute;stal</trd>
  </trdgrp>
  <trd lng="de">Ralle</trd>
  <trd lng="en">water-rail</trd>
  <trd lng="fr">r&acirc;le</trd>
  <trd lng="hu">guvat</trd>
  <trd lng="nl">ral</trd>
  <trd lng="pl">wodnik</trd>
  <trd lng="pt">ralo</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_u;&c_sh;&c_o;&c_k;
    <klr tip="amb">(&c_p;&c_t;&c_i;&c_c;&c_a;)</klr></trd>
  <trd lng="sk">chria&scaron;te&lcaron;</trd>
  <trd lng="uk">&#x43F;&#x430;&#x441;&#x442;&#x443;&#x448;&#x43E;&#x43A;</trd>
</drv>

<drv mrk="ral.akvo0o">
  <kap>akvo<tld/>o
    <fnt>
       <aut>Wouter F. Pilger</aut>,
       <vrk>
         <url ref="http://www.xs4all.nl/~pilger/wfp/birdeou8.htm">
           Komunlingvaj nomoj de E&ubreve;ropaj birdoj</url>
       </vrk>
    </fnt>
  </kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif> 
      <ref tip="lst" lst="voko:zoologiaj_specioj" cel="speci.0o">Specio</ref>
      de <ref tip="super" cel="ral.0o">ralo</ref> (<trd lng="la">Rallus aquaticus</trd>):
      <ekz>
        Akvoralo <klr>[havas]</klr> grizobluan viza&gcirc;on, gor&gcirc;on kaj bruston kun blankonigra vertikala
        striado &ccirc;eflanke
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=130663">Akvoralo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">r&acirc;le d'eau</trd>
    <trd lng="de">Wasserralle</trd>
    <trd lng="en">water <ind>rail</ind></trd>
    <trd lng="hu">guvat</trd>
    <trd lng="la">Rallus aquaticus</trd>
    <trd lng="nl">waterral</trd>
    <trd lng="pl">wodnik</trd>
    <trd lng="pt">frango d'&acirc;gua</trd>
    <trd lng="sv">vattenrall</trd>
  </snc>
  <trd lng="cs">ch&rcaron;&aacute;stal vodn&iacute;</trd>
  <trd lng="de">Wasserralle</trd>
  <trd lng="sk">chria&scaron;te&lcaron; vodn&yacute;</trd>
</drv>

<drv mrk="ral.0oformaj">
  <kap><tld/>oformaj</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref> el la klaso <ref tip="malprt" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Ralliformes</trd>):
      <ekz>
        la familio de raledoj grupi&gcirc;is tradicie <klr>[en]</klr> la 
        ordon de Gruoformaj, tamen la nova taksonomio de <nom>Sibley-Ahlquist</nom>, 
        akceptita en Usono, montras tiun birdogrupon kiel nova ordo, nome <tld lit="R"/>oformaj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=128231">Raledoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">Ralliformes</trd>
  <trd lng="de">Rallenv&ouml;gel</trd>
  <trd lng="fr">gruiformes</trd>
  <trd lng="hu">guvatalak&uacute;ak</trd>
  <trd lng="pl">chru&sacute;ciele</trd>
</drv>
</art>
