<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>hok</rad>/o
</kap>

<drv mrk="hok.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="hok.0o.gxenerale">
    <dif>
      Kurbigita, &gcirc;enerale pinta ferpeco, uzata por altiri al si ion
      a&ubreve; por pendigi ion sur &gcirc;i:
      <ekz>
        la blanka vesto, kiu pendas tie sur la <tld/>o
        <fnt><bib>Fab2</bib>, <lok>Ne&gcirc;a re&gcirc;ino</lok></fnt>;
      </ekz>
      <ekz>
        pendigu vian mantelon sur la <tld/>o;
      </ekz>
      <ekz>
        kurbi&gcirc;u <tld/>o la&ubreve; postuloj de l' loko
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        faru kvindek orajn <tld/>etojn, kaj kunigu la tapi&scirc;ojn unu kun la
        alia per la <tld/>etoj
        <fnt><bib>MT</bib>, <lok>&Eli; 26:6</lok></fnt>;
      </ekz>
      <ekz>
        sur la breto sub la vest<tld/>aro ku&scirc;is vintra &ccirc;apo
        <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 4</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[la kajerojn mi starigis]</klr> inter la &scirc;tipojn kaj per la
        <ind>fajro<tld/>o</ind>
        <klr>(<ref cel="fajr.0ostango">fajrostango</ref>)</klr>
        ta&ubreve;zis la foliojn
        <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 13</lok></fnt>.
        <trd lng="fr">tisonnier</trd>
        <trd lng="hu">piszkavas</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="agraf.0o">agrafo</ref>,
      <ref cel="krocx.0ilo">kro&ccirc;ilo</ref>.
    </refgrp>
    <trd lng="bg">&c_z;&c_a;&c_k;&c_a;&c_ch;&c_a;&c_l;&c_k;&c_a;</trd>
    <trd lng="br">krog</trd>
    <trd lng="de">Kleiderhaken</trd>
    <trdgrp lng="fr">
      <trd>crochet</trd>,
      <trd>pat&egrave;re</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>horog</trd>,
      <trd>kamp&oacute;</trd>,
      <trd>akaszt&oacute;</trd>,
    </trdgrp>
    <trdgrp lng="pt">
     <trd>gancho</trd>, 
     <trd>gafa</trd> 
    </trdgrp>
    <trdgrp lng="es">
     <trd>garfio</trd>, 
     <trd>gancho</trd> 
    </trdgrp>
  </snc>
  <snc mrk="hok.0o.pordhoko">
    <dif>
      <ref tip="dif" cel="pord.0hoko">Pord<tld/>o</ref>:
      <ekz>
        pordo turni&gcirc;as sur sia <tld/>o, kaj maldiligentulo sur sia lito
        <fnt><bib>MT</bib>, <lok>&Sen; 26:14</lok></fnt>;
      </ekz>
      <ekz>
        la pordoj de la pre&gcirc;ejo knarante turnas sin sur siaj <tld/>oj
        <fnt><bib>Fab1</bib>, <lok>Kupra porko</lok></fnt>;
      </ekz>
      <ekz>
        la pordo delevi&gcirc;is de unu <tld/>o kaj pendis tiel malrekte
        <fnt><bib>Fab2</bib>, <lok>Malbela anasido</lok></fnt>.
      </ekz>
    </dif>                  
    <trd lng="br">mudurun</trd>
    <trd lng="de">Angel <klr tip="ind">(T&uuml;r)</klr></trd>
    <trd lng="fr">gond</trd>
    <trd lng="hu">sarokvas<klr tip="ind"> (ajt&oacute;n)</klr></trd>
    <trd lng="pt">dobradi&ccedil;a</trd>
  </snc>
  <snc mrk="hok.0o.fisxhoko">
    <dif>
      <ref tip="dif" cel="fisx.0hoko">Fi&scirc;<tld/>o</ref>:
      <ekz>
        ankora&ubreve; la ezoko ne estas sur la <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        kapti fi&scirc;on sur <tld/>on<fnt>Z</fnt>.
      </ekz>
    </dif>                                            
    <trd lng="br">higenn</trd>
    <trd lng="de"><klr tip="ind">(T&uuml;r)</klr>Angel</trd>
    <trd lng="fr">hame&ccedil;on</trd>
    <trd lng="hu">horog</trd>
  </snc>
  <trd lng="de">Haken</trd>
  <trd lng="en">hook</trd>
  <trd lng="nl">haak</trd>
  <trd lng="pt">anzol</trd>
  <trd lng="es">anzuelo</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_ju;&c_k;</trd>,
    <trd>&c_k;&c_r;&c_ju;&c_ch;&c_o;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="hok.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Kurba kiel <tld/>o:
      <ekz>
        <ind><tld/>a nazo</ind>.         
        <trd lng="br">fri sparfell</trd>
      </ekz>
    </dif>
  </snc>      
  <trdgrp lng="br">
    <trd>krogek</trd>,
    <trd>kromm</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>aquilin</trd>,
    <trd>busqu&eacute;</trd>,
    <trd>crochu</trd>,
    <trd>recourb&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>horgas</trd>,
    <trd>kamp&oacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>haakvormig</trd>,
    <trd>krom</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>adunco</trd>, 
   <trd>curvo<klr tip="amb"> (como anzol)</klr></trd> 
  </trdgrp>
  <trdgrp lng="es">
   <trd>ganchudo, -a</trd>, 
   <trd>curvo<klr> (como anzuelo)</klr></trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_ju;&c_k;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_k;&c_r;&c_ju;&c_ch;&c_k;&c_o;&c_v;&c_a;&c_t;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="hok.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kro&ccirc;i per <tld/>o:
      <ekz>
        <tld/>i fi&scirc;on.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>kihorg&aacute;szik</trd>,
    <trd>beakaszt<klr tip="ind"> (horoggal)</klr></trd>,
    <trd>megfog<klr tip="ind"> (horoggal)</klr></trd>
  </trdgrp>
  <trd lng="nl">aan de <ind>haak</ind> slaan</trd>
  <trd lng="pt">fisgar</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_j;&c_m;&c_a;&c_t;&c_mol; &c_n;&c_a; <ind>&c_k;&c_r;&c_ju;&c_ch;&c_o;&c_k;</ind></trd>,
    <trd>&c_z;&c_a;&c_c;&c_je;&c_p;&c_i;&c_t;&c_mol; <ind>&c_k;&c_r;&c_ju;&c_ch;&c_k;&c_o;&c_m;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="hok.0fadeno0sxnuro">
  <kap><tld/>fadeno, <tld/>&scirc;nuro</kap>
  <snc>
    <dif>
      Fadeno provizita per <tld/>o(j) por fi&scirc;kapti.
    </dif>
  </snc>                                                         
  <trd lng="br">linenn<klr tip="ind"> (evit pesketa)</klr></trd>
  <trd lng="fr">ligne<klr tip="ind"> (pour la p&ecirc;che)</klr></trd>
  <trdgrp lng="hu">
    <trd>damil</trd>,
    <trd>horg&aacute;szzsin&oacute;r</trd>,
    <trd>horg&aacute;szzsineg</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vislijn</trd>,
    <trd>vissnoer</trd>
  </trdgrp>
  <trd lng="pt">linha de <ind>pesca</ind></trd>
  <trd lng="ru">&c_l;&c_je;&c_s;&c_k;&c_a;</trd>
</drv>

<drv mrk="hok.0stango">
  <kap><tld/>stango</kap>
  <snc>
    <dif>
      Bastono kun <tld/>o &ccirc;e la ekstrema&jcirc;o.
    </dif>
  </snc>             
  <trdgrp lng="br">
    <trd>bazh-krog</trd>,
    <trd>goaf<klr tip="ind"> (bazh-krog)</klr></trd>
  </trdgrp>        
  <trd lng="de">Hakenstange</trd>
  <trd lng="fr">gaffe<klr tip="ind"> (perche avec crochet)</klr></trd>
  <trd lng="hu">kamp&oacute;sbot</trd>
  <trd lng="nl">hengel</trd>
  <trd lng="ru">&c_b;&c_a;&c_g;&c_o;&c_r;</trd>
</drv>

<drv mrk="hok.fisx0i">
  <kap>fi&scirc;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fi&scirc;&ccirc;asi per <tld/>fadeno.
    </dif>
  </snc>                     
  <trd lng="br">pesketa<klr tip="ind"> (war-bouez un higenn)</klr></trd>
  <trd lng="de">angeln</trd>                                   
  <trd lng="fr">p&ecirc;cher<klr tip="ind"> (&agrave; la ligne)</klr></trd>
  <trd lng="hu">horg&aacute;szik</trd>
  <trdgrp lng="nl">
    <trd>vissen</trd>,
    <trd>hengelen</trd>
  </trdgrp>
  <trd lng="pt">fisgar</trd>
  <trd lng="ru">&c_u;&c_d;&c_i;&c_t;&c_mol; &c_r;&c_y;&c_b;&c_u;</trd>
</drv>
</art>
