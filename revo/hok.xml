<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>hok</rad>/o
</kap>
<drv mrk="hok.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="hok.0o.gxenerale">
    <dif>
      Kurbigita, &gcirc;enerale pinta ferpeco, uzata por altiri al si ion
      a&ubreve; por pendigi ion sur &gcirc;i:
      <ekz>
        la blanka vesto, kiu pendas tie sur la <tld/>o
        <fnt><bib>Fab2</bib>, <lok>Ne&gcirc;a re&gcirc;ino</lok></fnt>;
      </ekz>
      <ekz>
        pendigu vian mantelon sur la <tld/>o;
      </ekz>
      <ekz>
        kurbi&gcirc;u <tld/>o la&ubreve; postuloj de l' loko
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        faru kvindek orajn <tld/>etojn, kaj kunigu la tapi&scirc;ojn unu kun la
        alia per la <tld/>etoj
        <fnt><bib>MT</bib>, <lok>&Eli; 26:6</lok></fnt>;
      </ekz>
      <ekz>
        sur la breto sub la vest<tld/>aro ku&scirc;is vintra &ccirc;apo
        <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 4</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[la kajerojn mi starigis]</klr> inter la &scirc;tipojn kaj per la
        <ind>fajro<tld/>o</ind>
        <klr>(<ref cel="fajr.0ostango">fajrostango</ref>)</klr>
        ta&ubreve;zis la foliojn
        <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 13</lok></fnt>.
        <trd lng="fr">tisonnier</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="agraf.0o">agrafo</ref>,
      <ref cel="krocx.0ilo">kro&ccirc;ilo</ref>.
    </refgrp>
    <trdgrp lng="fr">
      <trd>crochet</trd>,
      <trd>pat&egrave;re</trd>
    </trdgrp>
  </snc>
  <snc mrk="hok.0o.pordhoko">
    <dif>
      <ref tip="dif" cel="pord.0hoko">Pord<tld/>o</ref>:
      <ekz>
        pordo turni&gcirc;as sur sia <tld/>o, kaj maldiligentulo sur sia lito
        <fnt><bib>MT</bib>, <lok>&Sen; 26:14</lok></fnt>;
      </ekz>
      <ekz>
        la pordoj de la pre&gcirc;ejo knarante turnas sin sur siaj <tld/>oj
        <fnt><bib>Fab1</bib>, <lok>Kupra porko</lok></fnt>;
      </ekz>
      <ekz>
        la pordo delevi&gcirc;is de unu <tld/>o kaj pendis tiel malrekte
        <fnt><bib>Fab2</bib>, <lok>Malbela anasido</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="fr">gond</trd>
  </snc>
  <snc mrk="hok.0o.fisxhoko">
    <dif>
      <ref tip="dif" cel="fisx.0hoko">Fi&scirc;<tld/>o</ref>:
      <ekz>
        ankora&ubreve; la ezoko ne estas sur la <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        kapti fi&scirc;on sur <tld/>on<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de"><klr tip="ind">(T&uuml;r)</klr>Angel</trd>
    <trd lng="fr">hame&ccedil;on</trd>
  </snc>
  <trd lng="de">Haken</trd>
  <trd lng="en">hook</trd>
  <trd lng="nl">haak</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_ju;&c_k;</trd>,
    <trd>&c_k;&c_r;&c_ju;&c_ch;&c_o;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="hok.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Kurba kiel <tld/>o:
      <ekz>
        <tld/>a nazo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>aquilin</trd>,
    <trd>busqu&eacute;</trd>,
    <trd>crochu</trd>,
    <trd>recourb&eacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>haakvormig</trd>,
    <trd>krom</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_ju;&c_k;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_k;&c_r;&c_ju;&c_ch;&c_k;&c_o;&c_v;&c_a;&c_t;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="hok.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kro&ccirc;i per <tld/>o:
      <ekz>
        <tld/>i fi&scirc;on.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">aan de <ind>haak</ind> slaan</trd>
</drv>

<drv mrk="hok.0fadeno0sxnuro">
  <kap><tld/>fadeno, <tld/>&scirc;nuro</kap>
  <snc>
    <dif>
      Fadeno provizita per <tld/>o(j) por fi&scirc;kapti.
    </dif>
  </snc>
  <trd lng="fr">ligne<klr tip="ind"> (pour la p&ecirc;che)</klr></trd>
  <trdgrp lng="nl">
    <trd>vislijn</trd>,
    <trd>vissnoer</trd>
  </trdgrp>
</drv>

<drv mrk="hok.0stango">
  <kap><tld/>stango</kap>
  <snc>
    <dif>
      Bastono kun <tld/>o &ccirc;e la ekstrema&jcirc;o.
    </dif>
  </snc>
  <trd lng="fr">gaffe<klr tip="ind"> (perche avec crochet)</klr></trd>
  <trd lng="nl">hengel</trd>
</drv>

<drv mrk="hok.fisx0i">
  <kap>fi&scirc;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fi&scirc;&ccirc;asi per <tld/>fadeno.
    </dif>
  </snc>
  <trd lng="de">angeln</trd>
  <trd lng="fr">p&ecirc;cher<klr tip="ind"> (&agrave; la ligne)</klr></trd>
  <trdgrp lng="nl">
    <trd>vissen</trd>,
    <trd>hengelen</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_d;&c_i;&c_t;&c_mol; &c_r;&c_y;&c_b;&c_u;</trd>
</drv>
</art>
