<drv mrk="lern.0ebleco">
  <kap><tld/>ebleco</kap>
  <snc mrk="lern.0ebleco.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      Pli a&ubreve; malpli ta&ubreve;geco, facileco de io por i&gcirc;i lernata:
      <ekz>
        la testoj celis esplori la &gcirc;eneralan <tld/>eblecon de Esperanto 
        kiel bazon por komparo kun aliaj lingvoj
        <fnt>
          <aut>W. Jansen</aut>,
          <vrk><url ref="http://edukado.net/biblioteko/dosierujo?iid=204">
          Pri Kelkaj Malfacila&jcirc;oj en Esperanto</url></vrk>,
          <lok>IPR, 2012:2, p. 26a-31a</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="en">learnability</trd>
  <trd lng="es">facilidad de aprendizaje</trd>
  <trd lng="fr">facilit&eacute; d'apprentissage</trd>
  <trdgrp lng="pl">
    <trd>mo&zdot;liwo&sacute;&cacute; nauczenia si&eogonek;</trd>,
    <trd>zdolno&sacute;ci {pl}</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>ocazie de a<ind>&icirc;nv&abreve;&#x21B;afaksu</ind></trd>,
    <trd>abilitate</trd>
  </trdgrp>
</drv>

