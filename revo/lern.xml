<drv mrk="lern.0ajxo">
  <kap><tld/>a&jcirc;o<fnt>Z</fnt></kap>
  <snc mrk="lern.0ajxo.PED">
    <dif>
      Materialo <tld/>ata a&ubreve; <tld/>ita:
      <ekz>
        tiuj dudekjaruloj <klr>[&#x2026;]</klr> devos kunigi la tre
        diversajn <tld/>a&jcirc;ojn kiujn ili <tld/>is infana&gcirc;e, tiel ke
        ili la&ubreve;orde komprenos la diversajn temojn kaj la naturon
        de la Realo
        <fnt>
          <aut>Platono, trad. D. Broadribb</aut>,
          <vrk>La Respubliko</vrk>,
          <lok>Parto 7a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de"><klr>(Lern-)</klr>Stoff</trd>
  <trd lng="es">material de <ind>aprendizaje</ind></trd>
  <trd lng="id">pel<ind>ajar</ind>an</trd>
  <trdgrp lng="pl">
    <trd>to, czego si&eogonek; <ind>uczy</ind></trd>,
    <trd>przedmiot nauki</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>ceea ce se &icirc;nva&#x21B;&abreve;</trd>,
    <trd>subiect de studiu</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_n;&c_a;&c_n;&c_i;&c_ja;<klr>
    (&c_p;&c_r;&c_i;&c_o;&c_b;&c_r;&c_je;&c_t;&c_jo;&c_n;&c_n;&c_y;&c_je;
    &c_u;&c_ch;&c_jo;&c_b;&c_o;&c_j;)</klr></trd>,
