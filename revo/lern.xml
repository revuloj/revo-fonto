      <trd lng="be">&c_p;&c_a;&c_d;&c_r;&c_u;&c_ch;&c_n;&c_ib;&c_k;</trd>
      <trd lng="bg">&c_u;&c_ch;&c_je;&c_b;&c_n;&c_i;&c_k;</trd>
      <trdgrp lng="br">
        <trd>levr-skol</trd>,
        <trd>dornlevr</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>livre de classe</trd>,
        <trd>manuel</trd>
      </trdgrp>
      <trd lng="de">Lehrbuch</trd>
      <trd lng="hu">tank&ouml;nyv</trd>
      <trd lng="nl">leerboek</trd>
      <trd lng="ru">&c_u;&c_ch;&c_je;&c_b;&c_n;&c_i;&c_k;</trd>
