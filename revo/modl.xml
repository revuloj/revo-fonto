<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>modl</rad>/i
</kap>
<drv mrk="modl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Formi el moligita materio iun plastikan objekton,
      kiun oni knedas kaj reliefigas per fingroj a&ubreve; per speciala ilo. 
    </dif>
  </snc>
  <trdgrp lng="nl">
    <trd>modelleren</trd>,
    <trd>boetseren</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_je;&c_p;&c_i;&c_t;&c_mol;</trd>
</drv>
<drv mrk="modl.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Speciala ilo por doni plastikan formon al moligita materio.  
    </dif>
  </snc>
  <trd lng="nl">boetseerhoutje</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_z;&c_je;&c_c;</trd>,
    <trd>&c_z;&c_u;&c_b;&c_i;&c_l;&c_o;</trd>
  </trdgrp>
</drv>
</art>
