<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>aviz</rad>/o
</kap>
<drv mrk="aviz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Sciigo, anonco, informo:
      <ekz>
        presigi <tld/>on en gazetoj;
      </ekz>
      <ekz>
        sciigi aferon per speciala <tld/>o;
      </ekz>
      <ekz>
        gasto sen <tld/>o estas agrabla surprizo<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_v;&c_je;&c_d;&c_a;&c_m;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>, 
    <trd>&c_a;&c_p;&c_a;&c_v;&c_ja;&c_sh;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>  
  <trdgrp lng="de">
    <trd>Anzeige</trd>, <trd>Anmeldung</trd>
  </trdgrp>
  <trd lng="en">notice</trd>
  <trdgrp lng="es">
    <trd>aviso</trd>,
    <trd>anuncio</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>avis <klr tip="ind">(annonce)</klr></trd>,
    <trd>annonce</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;rtes&iacute;t&eacute;s</trd>,
    <trd>k&ouml;zl&eacute;s</trd>,
    <trd>k&ouml;zlem&eacute;ny</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bericht</trd>,
    <trd>mededeling</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_v;&c_je;&c_d;&c_o;&c_m;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_i;&c_z;&c_v;&c_je;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_u;&c_p;&c_r;&c_je;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je; <klr>(&c_d;&c_o;&c_k;&c_u;&c_m;&c_je;&c_n;&c_t;)</klr></trd>,
    <trd>&c_a;&c_v;&c_i;&c_z;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="aviz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Sciigi pri io.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_v;&c_je;&c_d;&c_a;&c_m;&c_l;&c_ja;&c_c;&c_mol;</trd>, 
    <trd>&c_a;&c_p;&c_a;&c_v;&c_ja;&c_sh;&c_ch;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>  
  <trdgrp lng="en">
    <trd>notify</trd>, 
    <trd>serve notice</trd>, 
    <trd>put on notice</trd>
  </trdgrp> 
  <trd lng="de">anzeigen</trd>
  <trd lng="es">avisar</trd>
  <trd lng="fr">aviser</trd>
  <trd lng="hu">&eacute;rtes&iacute;t</trd>
  <trd lng="nl">meedelen</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_v;&c_je;&c_d;&c_o;&c_m;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol; &c_v; &c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_z;&c_v;&c_je;&c_shch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_u;&c_p;&c_r;&c_je;&c_zh;&c_d;&c_a;&c_t;&c_mol; <klr>(&c_p;&c_i;&c_s;&c_mol;&c_m;&c_o;&c_m; &c_i; &c_t;.&c_p;.)</klr></trd>
  </trdgrp>
</drv>
</art>
