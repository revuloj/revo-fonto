<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>aviz</rad>/o
</kap>
<drv mrk="aviz.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc>
     <dif>
       Sciigo, anonco, informo:
       <ekz>
         presigi <tld/>on en gazetoj;
       </ekz>
       <ekz>
         sciigi aferon per speciala <tld/>o;
       </ekz>
       <ekz>
         gasto sen <tld/>o estas agrabla surprizo
         <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="de"><trd>Anzeige</trd>, <trd>Anmeldung</trd></trdgrp>
   <trd lng="en">notice</trd>
   <trdgrp lng="es">
     <trd>aviso</trd>,
     <trd>anuncio</trd></trdgrp>
   <trdgrp lng="fr">
     <trd>avis <klr tip="ind">(annonce)</klr></trd>,
     <trd>annonce</trd></trdgrp>
   <trdgrp lng="ru">
     <trd>&#1091;&#1074;&#1077;&#1076;&#1086;&#1084;&#1083;&#1077;&#1085;&#1080;&#1077;</trd>,
     <trd>&#1080;&#1079;&#1074;&#1077;&#1097;&#1077;&#1085;&#1080;&#1077;</trd>,
     <trd>&#1087;&#1088;&#1077;&#1076;&#1091;&#1087;&#1088;&#1077;&#1078;&#1076;&#1077;&#1085;&#1080;&#1077; 
<klr>(&#1076;&#1086;&#1082;&#1091;&#1084;&#1077;&#1085;&#1090;)</klr></trd>,
     <trd>&#1072;&#1074;&#1080;&#1079;&#1086;</trd>
   </trdgrp>
</drv>
<drv mrk="aviz.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc>
     <dif>
       Sciigi pri io.
     </dif>
   </snc>
   <trd lng="de">anzeigen</trd>
   <trd lng="es">avisar</trd>
   <trd lng="fr">aviser</trd>
   <trdgrp lng="ru">
     <trd>&#1091;&#1074;&#1077;&#1076;&#1086;&#1084;&#1083;&#1103;&#1090;&#1100;</trd>,
     <trd>&#1089;&#1090;&#1072;&#1074;&#1080;&#1090;&#1100; &#1074; 
&#1080;&#1079;&#1074;&#1077;&#1089;&#1090;&#1085;&#1086;&#1089;&#1090;&#1100;</trd>,
     <trd>&#1087;&#1088;&#1077;&#1076;&#1091;&#1087;&#1088;&#1077;&#1078;&#1076;&#1072;&#1090;&#1100; 
<klr>(&#1087;&#1080;&#1089;&#1100;&#1084;&#1086;&#1084; &#1080; 
&#1090;.&#1087;.)</klr></trd>
   </trdgrp>
</drv>
</art>
