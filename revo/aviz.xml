<vortaro>
<art mrk="$Id: aviz.xml,v 1.37 2023/10/28 08:33:11 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>aviz</rad>/o
</kap>
<drv mrk="aviz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="aviz.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Afi&scirc;eto, etikedo, slipeto ks, sciiganta utilan
      informon a&ubreve; instrukciojn:
      <ekz>
        presigi <tld/>on en gazetoj;
      </ekz>
      <ekz>
        sciigi aferon per speciala <tld/>o;
      </ekz>
      <ekz>
        gasto sen <tld/>o estas agrabla 
        surprizo
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ho ve, ho mal&gcirc;ojo! <klr>[&#x2026;]</klr> ploregos patrinoj pri siaj infanoj,
        <klr>[&#x2026;]</klr> per cindro la griza ek&scirc;utos la kapojn &ccirc;e l' nigra <tld/>o
        <klr>(kp <ref cel="omen.0o">omeno</ref>, <ref cel="auxgur.0o">a&ubreve;guro</ref>)</klr>
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        ekzistas pli ol 20&#x202F;000 retpa&gcirc;oj pri sinmortigo kaj facile troveblas
        &leftquot;<tld/>otabuloj&rightquot; por tiuj, kiuj volas sin mortigi
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Yasuo</aut>
          <vrk><url ref="&Monato;/2003/008113.html">Forpaso-forumo</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="inform.0ilo">informilo</ref>
    <refgrp tip="vid">
      <ref cel="anonc.0o">anonco</ref>,
      <ref cel="afisx.0o">afi&scirc;o</ref>,
      <ref cel="bilet.0o">bileto</ref>,
      <ref cel="inform.0o.KOMUNE">informo</ref>,
      <ref cel="kupon.0o">kupono</ref>,
      <ref cel="kontra6.0o">kontramarko</ref>,
      <ref cel="sci.0igo">sciigo</ref>,
      <ref cel="not.diplomatia0o">diplomatia noto</ref>,
      <ref cel="memora.0o">memorando</ref>,
      <ref cel="direktiv.0o">direktivo</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Benachrichtigung</trd>,
      <trd>Anzeige</trd>,
      <trd>Anmeldung</trd>,
      <trd>Meldung</trd>,
      <trd>Anweisung</trd>,
      <trd>Visitenkarte</trd>,
      <trd>Hinweis</trd>,
      <trd>Warnschild</trd>,
      <trd>Formular</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_A;&a_ayn;&a_l;&a_A;&a_n;</trd>,
      <trd>&a_A_madda;&f_g;&a_h;&f_y;</trd>,
      <trd>&a_A;&a_T;&a_l;&a_A;&a_ayn;&f_y;&a_h;</trd>
    </trdgrp>
    <trd lng="it">avviso</trd>
    <trdgrp lng="pl">
      <trd>awizo</trd>,
      <trd>awizacja</trd>,
      <trd>zawiadomienie</trd>,
      <trd>powiadomienie</trd>,
      <trd>ostrze&zdot;enie</trd>,
      <trd>notatka</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x63D0;&#x793A; <pr>t&#xED;sh&#xEC;</pr></trd>,
    <trd>&#x8A3C;&#x660E; <pr>zh&#xE8;ngm&#xED;ng</pr></trd>,
    <trd>&#x8BC1;&#x660E; <pr>zh&#xE8;ngm&#xED;ng</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="aviz.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Koresponda&jcirc;o per kiu oni sciigas al iu, ke ties
      senda&jcirc;o atingis  la celon:
      <ekz>
        <tld/>o pri ricevo de registrita letero
        <fnt><bib>PIV2</bib></fnt>;
      </ekz>
      <ekz>
        fervoja <tld/>o pri alveninta paka&jcirc;o
        <fnt><bib>PIV2</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="konfir.0o.certigi">konfirmo<sncref/></ref>
    <trd lng="de">Empfangsbest&auml;tigung</trd>
    <trd lng="it">notifica</trd>
    <trdgrp lng="pl">
      <trd>awizo</trd>,
      <trd>awizacja</trd>,
      <trd>zawiadomienie</trd>,
      <trd>powiadomienie</trd>
    </trdgrp>  
    <trdgrp lng="zh">
    <trd>&#x56DE;&#x57F7; <pr>hu&#xED;zh&#xED;</pr></trd>,
    <trd>&#x56DE;&#x6267; <pr>hu&#xED;zh&#xED;</pr></trd>,
    <trd>&#x6536;&#x636E; <pr>sh&#x14D;uj&#xF9;</pr></trd>,
    <trd>&#x6536;&#x64DA; <pr>sh&#x14D;uj&#xF9;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>
    &c_p;&c_a;&c_v;&c_je;&c_d;&c_a;&c_m;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;
    </trd>, 
    <trd>
    &c_a;&c_p;&c_a;&c_v;&c_ja;&c_sh;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;
    </trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_u;&c_v;&c_je;&c_d;&c_o;&c_m;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_o;&c_b;&c_ja;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="br">kemennadenn</trd>
  <trdgrp lng="cs">
    <trd>av&iacute;zo</trd>,
    <trd>n&aacute;v&ecaron;&scaron;t&iacute;</trd>,
    <trd>upozorn&ecaron;n&iacute;</trd>,
    <trd>vyrozum&ecaron;n&iacute;</trd>,
    <trd>v&yacute;zva</trd>,
    <trd>zpr&aacute;va</trd>
  </trdgrp>
  
  <trdgrp lng="en">
    <trd>notice</trd>,
    <trd>memo</trd>,
    <trd>post</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>aviso</trd>,
    <trd>anuncio</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>avis <klr tip="ind">(annonce)</klr></trd>,
    <trd>annonce</trd>
  </trdgrp>
  <trd lng="he">&#x5D4;&#x5D5;&#x5D3;&#x5E2;&#x5D4;</trd>
  <trdgrp lng="hu">
    <trd>&eacute;rtes&iacute;t&eacute;s</trd>,
    <trd>k&ouml;zl&eacute;s</trd>,
    <trd>k&ouml;zlem&eacute;ny</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x901A;&#x77E5;  <pr>&#x3064;&#x3046;&#x3061;</pr></trd>,
    <trd>&#x544A;&#x77E5;  <pr>&#x3053;&#x304F;&#x3061;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bericht</trd>,
    <trd>mededeling</trd>
  </trdgrp>
    <trd lng="pt">aviso</trd>
  <trdgrp lng="ru">
  <trd>
  &c_u;&c_v;&c_je;&c_d;&c_o;&c_m;&c_l;&c_je;&c_n;&c_i;&c_je;
  </trd>,
  <trd>
  &c_i;&c_z;&c_v;&c_je;&c_shch;&c_je;&c_n;&c_i;&c_je;
  </trd>,
  <trd>&c_p;&c_r;&c_je;&c_d;&c_u;&c_p;&c_r;&c_je;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;  
  <klr>(&c_d;&c_o;&c_k;&c_u;&c_m;&c_je;&c_n;&c_t;)</klr></trd>,
  <trd>&c_a;&c_v;&c_i;&c_z;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>av&iacute;zo</trd>,
    <trd>ozn&aacute;menie</trd>,
    <trd>spr&aacute;va</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&c_v;&c_ib;&c_d;&#x43E;&#x43C;&#x43B;&c_je;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43F;&#x43E;&#x43F;&c_je;&c_r;&c_je;&c_d;&c_zh;&c_je;&#x43D;&#x43D;&#x44F;&#xA0;(&c_d;&#x43E;&#x43A;&c_u;&#x43C;&c_je;&#x43D;&c_t;)</trd>,
    <trd>&c_a;&c_v;&c_ib;&c_z;&#x43E;</trd>
  </trdgrp>
</drv>

<drv mrk="aviz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="aviz.0i.sciigi">
    <dif>
      Sciigi, informi pri io per <tld/>o:
      <ekz>
        <klr>[la]</klr> komisiono <klr>[&#x2026;]</klr> devas informi la episkopojn kaj <tld/>i
        ilin pri necesaj pa&scirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="&Monato;/2010/010119.php">&Ccirc;u viktimoj de justico?</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la pa&gcirc;oj de tiu &ccirc;i revuo anka&ubreve; <nom>Mark Fettes</nom> avizis &leftquot;novan epokon&rightquot; sub
        <nom>Valle</nom>
        kaj ju&gcirc;is la tempon de <nom>Stano Mar&ccaron;ek</nom> tiel negative, ke lia nediplomatia
        vangofrapo vere ne decis al la prezidanto de UEA
        <fnt>
          <vrk><url ref="&LOdE;Ondo/233-lode.htm">La Ondo de Esperanto, 2014, 3</url></vrk>
        </fnt>.
      </ekz>
    </dif>  
    <refgrp tip="sin">
      <ref cel="anonc.0i">anonci</ref>,
      <ref cel="avert.0i">averti</ref>,
      <ref cel="sci.0igi">sciigi</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>
      &c_p;&c_a;&c_v;&c_je;&c_d;&c_a;&c_m;&c_l;&c_ja;&c_c;&c_mol;
      </trd>, 
      <trd>&c_a;&c_p;&c_a;&c_v;&c_ja;&c_sh;&c_ch;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>  
    <trd lng="br">kemenn<klr tip="ind"> (v.)</klr></trd>
    <trdgrp lng="en">
      <trd>notify</trd>, 
      <trd>serve notice</trd>, 
      <trd>put on notice</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>anzeigen</trd>,
      <trd>anmelden</trd>,
      <trd>anweisen</trd>,
      <trd>hinweisen</trd>,
      <trd>melden</trd>,
      <trd>warnen</trd>
    </trdgrp>
    <trd lng="es">avisar</trd>
    <trdgrp lng="fa">
      <trd>&a_A_madda;&f_g;&a_A;&a_h; &f_k;&a_r;&a_d;&a_n;</trd>,
      <trd>&a_A;&a_T;&a_l;&a_A;&a_ayn; &a_d;&a_A;&a_d;&a_n;</trd>
    </trdgrp>
    <trd lng="fr">aviser</trd>
    <trd lng="hu">&eacute;rtes&iacute;t</trd>
    <trd lng="it">avvisare</trd>
    <trd lng="nl">meedelen</trd>
    <trdgrp lng="pl">
      <trd>awizowa&cacute;</trd>,
      <trd>zawiadomia&cacute;</trd>,
      <trd>powiadomia&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_u;&c_v;&c_je;&c_d;&c_o;&c_m;&c_l;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol; &c_v;
  &c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_i;&c_z;&c_v;&c_je;&c_shch;&c_a;&c_t;&c_mol;</trd>,
      <trd>
      &c_p;&c_r;&c_je;&c_d;&c_u;&c_p;&c_r;&c_je;&c_zh;&c_d;&c_a;&c_t;&c_mol; 
      <klr>(&c_p;&c_i;&c_s;&c_mol;&c_m;&c_o;&c_m; &c_i;
      &c_t;.&c_p;.)</klr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_u;&c_v;&c_je;&c_d;&c_o;&c_m;&c_ja;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_ja;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>avizovat</trd>,
    <trd>ozn&aacute;mit</trd>,
    <trd>upozornit n&aacute;v&ecaron;&scaron;t&iacute;m</trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5D4;&#x5D5;&#x5D3;&#x5D9;&#x5E2; &#x5D1;&#x5DB;&#x5EA;&#x5D1;</trd>
  <trdgrp lng="ja">
    <trd>&#x901A;&#x77E5;&#x3059;&#x308B;  <pr>&#x3064;&#x3046;&#x3061;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x544A;&#x77E5;&#x3059;&#x308B;  <pr>&#x3053;&#x304F;&#x3061;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="sk">vopred ozn&aacute;mi&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&c_v;&c_ib;&c_d;&#x43E;&#x43C;&#x43B;&#x44F;&c_t;&c_i;&#xA0;(&#x43A;&#x43E;&c_g;&#x43E;-&#x43D;.
    &#x43F;&c_r;&#x43E; &c_shch;&#x43E;-&#x43D;.)</trd>,
    <trd>&c_d;&#x43E;&c_v;&#x43E;&c_d;&c_i;&c_t;&c_i; &c_d;&#x43E; &c_v;&c_ib;&c_d;&#x43E;&#x43C;&c_a;</trd>,
    <trd>&#x43F;&#x43E;&#x43F;&c_je;&c_r;&c_je;&c_d;&c_zh;&c_a;&c_t;&c_i;&#xA0;(&#x43B;&c_i;&c_s;&c_t;&#x43E;&#x43C; &c_ib; &c_t;.&#x43F;.)</trd>
  </trdgrp>
</drv>
</art>
