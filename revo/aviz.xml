<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>aviz</rad>/o
</kap>
<drv mrk="aviz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Sciigo, anonco, informo:
      <ekz>
        presigi <tld/>on en gazetoj;
      </ekz>
      <ekz>
        sciigi aferon per speciala <tld/>o;
      </ekz>
      <ekz>
        gasto sen <tld/>o estas agrabla surprizo
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>.
      </ekz> 
    </dif>
  </snc>
  <trdgrp lng="de"><trd>Anzeige</trd>, <trd>Anmeldung</trd></trdgrp>
  <trd lng="en">notice</trd>
</drv>
<drv mrk="aviz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Sciigi pri io.  
    </dif>
  </snc>
  <trd lng="de">anzeigen</trd>
</drv>
</art>
<!--
$Log$
-->
</vortaro>