<drv mrk="bezon.0o">
  <kap><tld/>o</kap>
  <snc mrk="bezon.0o.sufero">
    <dif>
      Sufero pro manko de io necesa:
      <ekz>
        kiu tro <tld/>as, tiu le&gcirc;on ne konas
        <fnt>
          <bib>Prv</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estas plej forta ordono<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="hu">sz&uuml;ks&eacute;g</trd>
  </snc>
  <snc mrk="bezon.0o.postulo">
    <dif>
      Stato, kiu postulas ion, kiel necesan, la&ubreve;celan
      a&ubreve; utilan:
      <ekz>
        klareco estas <tld/>o de normala spirito;
      </ekz><ekz>
        en okazo de <tld/>o
        <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 16</lok></fnt>;
      </ekz><ekz>
        la uzado akuzativon en kazo de sen<tld/>o
        <fnt>
          <aut>Andre CHERPILLOD</aut>
          <vrk>La influo de la hebrea lingvo
          sur Esperanto</vrk>
          <bib>Monato</bib>
          <lok>2000/12, p. 18</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">sz&uuml;ks&eacute;g</trd>
    <trd lng="zh">&#x8FEB;&#20999;&#38656;&#35201; <pr>p&#xF2;qi&#xE8;x&#x16B;y&#xE0;o</pr></trd>
</snc>
  <snc mrk="bezon.0o.manko">
    <dif>
      <ref tip="dif" cel="bezon.0ajxo.KOMUNE"><tld lit="B"/>a&jcirc;o</ref>:
      <ekz>
        havi grandajn, malmultajn, moderajn <tld/>ojn;
      </ekz><ekz>
        La <tld/>oj de l' kortego estis malavare provizitaj
        <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 9a.</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="hu">sz&uuml;ks&eacute;glet</trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_t;&c_r;&c_e;&c_b;&c_a;</trd>
  <trdgrp lng="br">
    <trd>ezhomm</trd>,
    <trd>c'hoant</trd>
  </trdgrp>
  <trd lng="ca">necessitat</trd>
  <trdgrp lng="cs">
    <trd>n&aacute;le&zcaron;itost <klr>(co je nutn&eacute;)</klr></trd>,
    <trd>pot&rcaron;eba</trd>,
    <trd>pot&rcaron;ebn&aacute; v&ecaron;c</trd>,
    <trd>po&zcaron;adavek</trd>
  </trdgrp>
  <trd lng="de">Bed&uuml;rfnis</trd>
  <trdgrp lng="en">
    <trd>need</trd>,
    <trd>want</trd></trdgrp>
  <trd lng="es">necesidad</trd>
  <trdgrp lng="fr">
    <trd>besoin</trd>,
    <trd>n&eacute;cessit&eacute;</trd></trdgrp>
  <trd lng="he">&#x5E6;&#x5D5;&#x5B9;&#x5E8;&#x5B6;&#x5DA;</trd>
  <trdgrp lng="id">
    <trd><baz>butuh</baz>kebutuhan</trd>,
    <trd><baz>perlu</baz>keperluan</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5FC5;&#35201;  <pr>&#12402;&#12388;&#12424;&#12358;</pr></trd>,
    <trd>&#x4E0D;&#x8DB3;  <pr>&#12405;&#x305D;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="oc">besonh</trd>
  <trdgrp lng="pl">
     <trd>potrzeba</trd>,
     <trd>zapotrzebowanie</trd>
   </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_u;&c_zh;&c_d;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_t;&c_r;&c_je;&c_b;&c_n;&c_o;&c_s;&c_t;&c_mol;
    </trd>
  </trdgrp>
  <trd lng="sk">potrebn&aacute; vec</trd>
  <trd lng="sv">behov</trd>
  <trd lng="tok">wile</trd>
  <trd lng="uk">&#x43F;&#x43E;&c_t;&c_r;&c_je;&c_b;&c_a;</trd>
