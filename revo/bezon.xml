<drv mrk="bezon.0a">
  <kap><tld/>a</kap>
  <snc mrk="bezon.0a.bezonata">
    <dif>
      <tld lit="B"/>ata, havenda por iu celo:
      <ekz>
        &Ccirc;u lingvo internacia estas <tld/>a?
        <fnt><bib>EE</bib>, <lok>&ccirc;ap. 2a</lok></fnt>;
      </ekz><ekz>
        <uzo tip="stl">FRAZ</uzo>
        por vino bona &scirc;ildo <klr>(reklamo)</klr>
        ne <tld/>a;
      </ekz><ekz>
        <uzo tip="stl">FRAZ</uzo>
        kiu avidas pli bonan, perdas plej <tld/>an
        <fnt><bib>PrV</bib></fnt>;
      </ekz><ekz>
        <uzo tip="stl">FRAZ</uzo>
        &scirc;paru kiam bone, vi havos kiam <tld/>e
        <fnt><bib>PrV</bib></fnt>;
      </ekz><ekz>
        la a&jcirc;ojn tute sen<tld/>ajn mi simple for&jcirc;etis, dum la a&jcirc;ojn ja
        <tld/>ajn, sed &ccirc;i-vintre ne <tld/>atajn mi metis en la
        kameron;
      </ekz><ekz>
        5 jaroj estis <tld/>aj por venki ilin.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="util.0a">utila</ref>,
      <ref cel="neces.0a">necesa</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_t;&c_r;&c_e;&c_b;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_a;&c_b;&c_h;&c_o;&c_d;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>rekis</trd>,
    <trd>ezhommek<klr tip="ind"> (trao&ugrave;)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nutn&yacute;</trd>,
    <trd>pot&rcaron;ebn&yacute;</trd>
  </trdgrp>
  <trd lng="de">n&ouml;tig</trd>
  <trdgrp lng="en">
    <trd>needed</trd></trdgrp>
  <trd lng="es">necesario</trd>
  <trd lng="fr">n&eacute;cessaire</trd>
  <trd lng="he">&#x5E0;&#x5D7;&#x5D5;&#x5E5;</trd>
  <trd lng="hu">sz&uuml;ks&eacute;ges</trd>
  <trdgrp lng="id">
    <trd><baz>butuh</baz>dibutuhkan</trd>,
    <trd><baz>perlu</baz>diperlukan</trd>
  </trdgrp>
  <trd lng="ja">&#x5FC5;&#35201;&#x306A;  <pr>&#12402;&#12388;&#12424;&#12358;&#x306A;</pr></trd>
  <trdgrp lng="pl">
    <trd>potrzebny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_u;&c_zh;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_b;&c_u;&c_ju;&c_shch;&c_i;&c_j;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nutn&yacute;</trd>,
    <trd>potrebn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20999;&#35201; <pr>qi&#xE8;y&#xE0;o</pr></trd>,
    <trd>&#x5FC5;&#35201; <pr>b&#xEC;y&#xE0;o</pr></trd>,
    <trd>&#x4E0D;&#x53EF;&#25110;&#x7F3A; <pr>b&#xF9;k&#x11B;hu&#xF2;qu&emacron;</pr></trd>,
    <trd>&#25152;&#38656; <pr>su&#x1D2;x&#x16B;</pr></trd>,
    <trd>&#x5FC5;&#20633; <pr>b&#xEC;b&#xE8;i</pr></trd>
  </trdgrp>
