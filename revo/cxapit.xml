  <trd lng="bg">&c_g;&c_l;&c_a;&c_v;&c_a; &c_n;&c_a; &c_k;&c_n;&c_i;&c_g;&c_a;</trd>
  <trd lng="bo">&#xF63;&#xF7A;&#xF60;&#xF74;&#xF0B;</trd>
