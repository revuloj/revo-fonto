  <trd lng="hu">fejezet</trd>
  <trd lng="it">capitolo</trd>
  <trd lng="nl">hoofdstuk</trd>
  <trd lng="pt">cap&iacute;tulo</trd>
  <trd lng="ru">&c_g;&c_l;&c_a;&c_v;&c_a; <klr>(&c_k;&c_n;&c_i;&c_g;&c_i;)</klr></trd>
  <trd lng="sk">kapitola</trd>
  <trd lng="sv">kapitel</trd>
  <trd lng="vo">kapit</trd>
</drv>
</art>
