<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&ccirc;apitr</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="cxapit.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="cxapit.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Divida&jcirc;o de libro, dokumento, signita per iu titolo a&ubreve; numero:
      <ekz>
        la libro estas organizita en dek na&ubreve; <tld/>oj
        <fnt>
          <aut>C. Kiselman</aut>,
          <vrk>La anta&ubreve;iranto de Esperanto (A. Cherpillod:
          Konciza gramatiko de Volapuko)</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 1995a, numero 9a, p. 23a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_d;&c_z;&c_je;&c_l;,</trd>,
    <trd>&c_ch;&c_a;&c_s;&c_t;&c_k;&c_a;<klr>(&c_k;&c_n;&c_ib;&c_g;&c_ib;)</klr></trd>
  </trdgrp>
  <trd lng="de">Kapitel</trd>
  <trd lng="el">&kappa;&epsilon;&phi;&alfa_ton;&lambda;&alfa;&jota;&omikron;</trd>
  <trd lng="en">chapter</trd>
  <trd lng="es">cap&iacute;tulo</trd>
  <trd lng="fr">chapitre</trd>
  <trd lng="he">&pe;&resh;&qof;</trd>
  <trd lng="hu">fejezet</trd>
  <trd lng="it">capitolo</trd>
  <trd lng="nl">hoofdstuk</trd>
  <trd lng="pt">cap&iacute;tulo</trd>
  <trd lng="ru">&c_g;&c_l;&c_a;&c_v;&c_a; <klr>(&c_k;&c_n;&c_i;&c_g;&c_i;)</klr></trd>
  <trd lng="sk">kapitola</trd>
  <trd lng="sv">kapitel</trd>
  <trd lng="vo">kapit</trd>
</drv>
</art>
