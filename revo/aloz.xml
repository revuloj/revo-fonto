<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>aloz</rad>/o
</kap>
<drv mrk="aloz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="klupee.0oj">klupeedoj</ref>
      (<trd lng="la">Alosa</trd>), kiu migras por frajado el la maro en
      la riverojn.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Alse</trd>,
    <trd>Maifisch</trd>
  </trdgrp>
  <trd lng="en">shad</trd>
  <trd lng="es">alosa</trd>
  <trd lng="fr">alose</trd>
  <trd lng="it">alaccia</trd>
  <trdgrp lng="nl">
    <trd>meivis</trd>,
    <trd>elft</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>alosa</trd>,
    <trd>s&aacute;vel</trd>,
    <trd>savelha</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_l;&c_mol;&c_d;&c_mol;-&c_p;&c_u;&c_z;&c_a;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_je;&c_d;</trd>
  </trdgrp>
</drv>
</art>
