<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
 
<vortaro>
<art mrk="$Id$">
<kap>
<rad>aloz</rad>/o
</kap>
<drv mrk="aloz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="klupe.0edoj">klupeedoj</ref>
      (<trd lng="la">Alosa</trd>), kiu migras por frajado el la maro en
      la riverojn.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Alse</trd>,
    <trd>Maifisch</trd>
  </trdgrp>    
  <trd lng="en">shad</trd>
  <trd lng="es">alosa</trd>
  <trd lng="fr">alose</trd>
  <trd lng="it">alaccia</trd>
  <trdgrp lng="nl">
    <trd>meivis</trd>,
    <trd>elft</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>s&aacute;vel</trd>,
    <trd>savelha</trd>
  </trdgrp>
</drv>
</art>

