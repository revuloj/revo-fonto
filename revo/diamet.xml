<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: diamet.xml,v 1.27 2023/11/04 14:29:01 revo Exp $">
<kap>
<ofc>1</ofc>
<rad>diametr</rad>/o
</kap>
<drv mrk="diamet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="diamet.0o.cirklo">
    <fnt><bib>MatTerm</bib><lok>p. 28</lok></fnt>
    <dif>
      <klr>(de <ref cel="cirkl.0o.linio">cirklo</ref> a&ubreve;
      <ref cel="sfer.0o.surfaco">sfero</ref>)</klr>
      &Ccirc;iu el la <ref tip="super" cel="strek.0o.segmento">strekoj</ref>,
      kunligantaj du punktojn de la koncerna figuro, kaj trairantaj
      &gcirc;ian
      <ref cel="centr.0o.cirklo">centron</ref>; la longo de tia streko:
      <ekz>
        <tld/>oj de cirklo estas &gcirc;iaj plej longaj
        <ref cel="sxnur.0o.cirklo">&scirc;nuroj</ref>;
      </ekz>
      <ekz>
        <tld/>o egalas al duoblo de <ref cel="radius.0o.cirklo">radiuso</ref>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="diamet.0o.subaro">
    <fnt><bib>EKV</bib><lok>&para;78</lok></fnt>
    <dif>
      <klr>(de subaro de <ref cel="spac.metrika0o">metrika
      spaco</ref>)</klr>
      <ref cel="suprem.0o">Supremo</ref> de la distancoj inter
      du &gcirc;iaj punktoj:
      <ekz>
        konvencie oni diras, ke <ref cel="bar.0ita.metrikaspaco">nebarita</ref>
        subaro havas nefinian <tld/>on.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_m;&c_e;&c_t;&c_a;&c_r;</trd>
  <trd lng="bg">&c_d;&c_i;&c_a;&c_m;&c_je;&c_t;&c_malmol;&c_r;</trd>
  <trd lng="ca">di&agrave;metre</trd>
  <trd lng="cs">pr&#x16F;m&#x11B;r kru&#x17E;nice</trd>
  <trd lng="de">Durchmesser</trd>
  <trd lng="en">diameter</trd>
  <trd lng="es">di&aacute;metro</trd>
  <trd lng="fa">&a_q;&a_T;&a_r;</trd>
  <trd lng="fr">diam&egrave;tre</trd>
  <trd lng="he">&#x5E7;&#x5D5;&#x5B9;&#x5D8;&#x5B6;&#x5E8;</trd>
  <trd lng="hu">&aacute;tm&eacute;r&odblac;</trd>
  <trd lng="ja">&#x76F4;&#x5F84;  <pr>&#x3061;&#x3087;&#x3063;&#x3051;&#x3044;</pr></trd>
  <trd lng="pl">&sacute;rednica</trd>
  <trd lng="pt">di&acirc;metro</trd>
  <trd lng="ru">&c_d;&c_i;&c_a;&c_m;&c_je;&c_t;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>priemer <klr>(hodn&#xF4;t)</klr></trd>,
    <trd>priemer <klr>(kruhu)</klr></trd>
  </trdgrp>
  <trd lng="sv">diameter</trd>
  <trd lng="uk">&#x434;&#x456;&#x430;&#x43C;&#x435;&#x442;&#x440;</trd>
  <trdgrp lng="zh">
    <trd>&#x5F84; <pr>j&#xEC;ng</pr></trd>,
    <trd>&#x4E2D;&#x5F84; <pr>zh&#x14D;ngj&#xEC;ng</pr></trd>,
    <trd>&#x76F4;&#x5F84; <pr>zh&#xED;j&#xEC;ng</pr></trd>,
    <trd>&#x771F;&#x5F84; <pr>zh&#x113;nj&#xEC;ng</pr></trd>
  </trdgrp>
</drv>
</art>
