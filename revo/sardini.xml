<drv mrk="sardini.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="sard.S0ujo">Sardujo</ref>:
      <ekz>
        neniel honorindan rekordon tenas la malliberejo de
        Kaljaro, en <tld/>o, en kiu
        ekde 2005 prenis sian vivon 11 el la 50 malliberuloj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.be/2010/010161.php"
              >Rajto je minimuma digno</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        vizitu sentime <tld/>on, kiu estas <klr>[&#8230;]</klr>
        vere rava insulo
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.be/2013/010777.php"
              >&Gcirc;is la tero tremas</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Sardinien</trd>
    <trd lng="en">Sardinia</trd>
    <trd lng="fr">Sardaigne</trd>
  </snc>
</drv>

