<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>helvel</rad>/o
</kap>
<drv mrk="helvel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="helvel.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Helvella</trd>), fungo man&gcirc;ebla nur post
      kuirado kaj forver&scirc;o de la kuirakvo.
    </dif>
  </snc>
  <trd lng="de">Lorchel</trd>
</drv>
<drv mrk="helvel.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="fung.sak0oj">sakfungoj</ref>
      (<trd lng="la">Helvellaceae</trd>).
    </dif>
  </snc>
</drv>
</art>
