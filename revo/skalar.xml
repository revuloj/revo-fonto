<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
   <art mrk="$Id$">
    <kap>
      <rad>skalar</rad>/o
    </kap>
    <drv mrk="skalar.0o">
      <kap><tld/>o<fnt>PIV</fnt></kap>
      <snc mrk="skalar.0o.MAT">
        <uzo tip="fak">MAT</uzo>
        <dif>
          <ref tip="super" cel="elemen.0o.MAT">Elemento</ref> de
          <ref cel="ring.0o.MAT">ringo</ref> a&ubreve;
          <ref cel="korp.0o.MAT">korpo</ref>, kiu operacias
          super
          <ref cel="modul.0o.magmo">modulo</ref> a&ubreve;
          <ref tip="malprt" cel="spac.vektora0o">vektora spaco</ref>.
        </dif>
        <trd lng="ru">&c_s;&c_k;&c_a;&c_l;&c_ja;&c_r;</trd>
      </snc>
      <snc mrk="skalar.0o.FIZ">
        <uzo tip="fak">FIZ</uzo>
        <dif>
          Fizika grando, kiu ne havas direkton; la reelo &gcirc;in
          reprezentanta:
          <ekz>
            la grando temperaturo estas <tld/>o.
          </ekz>
        </dif>
        <ref tip="ant" cel="vektor.0o.FIZ">vektoro.</ref>
      </snc>
      <trd lng="fr">scalaire</trd>
    </drv>

   </art>


<!--
$Log$
-->
</vortaro>