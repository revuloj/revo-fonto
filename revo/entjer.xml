<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: entjer.xml,v 1.29 2023/11/07 20:34:10 revo Exp $">
<kap>
  <rad>entjer</rad>/o
</kap>
<drv mrk="entjer.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 7</lok></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="nombr.0o.MAT">Nombro</ref> el la sinsekvo
      <frm am="0,1,2,3,...">0, 1, 2, 3,...</frm> a&ubreve; el la sinsekvo
      <frm am="-1,-2,-3,...">-1, -2, -3,...</frm>:
      <ekz>
        la aron de <tld/>oj oni signas per <frm am="ZZ"><g>Z</g></frm>.
      </ekz>
    </dif>
    <ref tip="super" cel="racion.0o">racionalo</ref>
    <refgrp tip="vid">
      <ref cel="aritme.0o">aritmetiko</ref>,
      <ref cel="kardin.0o.MAT">kardinalo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_c;&c_e;&c_l;&c_y; <ind>&c_l;&c_ib;&c_k;</ind></trd>
  <trdgrp lng="cs">
    <trd>celek</trd>,
    <trd>cel&eacute; &ccaron;&iacute;slo</trd>
  </trdgrp>
  <trd lng="de">ganze <ind>Zahl</ind></trd>
  <trdgrp lng="en">
    <trd>integer</trd>,
    <trd>integral <ind>number</ind></trd></trdgrp>
  <trd lng="es">entero</trd>
  <trdgrp lng="fr">
    <trd>entier<klr tip="ind"> (nombre)</klr></trd>,
    <trd>nombre entier</trd>,
    <trd>entier relatif</trd>,
    <trd>entier rationnel</trd>
  </trdgrp>
  <trd lng="hu">eg&eacute;sz <ind>sz&aacute;m</ind></trd>
  <trd lng="ja">&#x6574;&#x6570;  <pr>&#x305B;&#x3044;&#x3059;&#x3046;</pr></trd>
  <trd lng="pl">liczba ca&lstroke;kowita</trd>
  <trd lng="pt">inteiro</trd>
  <trd lng="ru">&c_c;&c_je;&c_l;&c_o;&c_je;
<ind>&c_ch;&c_i;&c_s;&c_l;&c_o;</ind></trd>
  <trd lng="sk">cel&eacute; &ccaron;&iacute;slo</trd>
  <trd lng="sv">heltal</trd>
  <trdgrp lng="uk">
    <trd>&#x446;&#x456;&#x43B;&#x435; &#x447;&#x438;&#x441;&#x43B;&#x43E;</trd>,
    <trd>&#x430;&#x43D;&#x442;&#x44C;&#x454;</trd>
  </trdgrp>
  <trd lng="zh">&#x6574;&#x578B; <pr>zh&#x11B;ngx&#xED;ng</pr></trd>
</drv>

<drv mrk="entjer.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 7</lok></fnt>
  <snc>
    <dif>
      Rilata al <tld/>o, estanta <tld/>o:
      <ekz>
        <tld/>a nombro <klr>(<tld/>o)</klr>,
      </ekz>
      <ekz>
        variablo,
      </ekz>
      <ekz>
        funkcio <klr>(kies valoroj estas <tld/>aj)</klr>;
      </ekz>
      <ekz>
        <ref cel="part.entjera0o">entjera parto</ref> de reelo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_e;&c_l;&c_y;</trd>,
    <trd>&c_c;&c_e;&c_l;&c_a;&c_l;&c_ib;&c_k;&c_a;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>digit&aacute;ln&iacute;</trd>,
    <trd>&ccaron;&iacute;seln&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ganz</trd>,
    <trd>ganzwertig</trd>,
    <trd>ganzzahlig</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>integer</trd>,
    <trd>integral<klr tip="ind"> (integer)</klr></trd>,
    <trd>integer-valued</trd>
  </trdgrp>
  <trd lng="fr">entier<klr tip="ind"> (adj., math.)</klr></trd>
  <trd lng="hu">eg&eacute;sz<klr tip="ind"> (sz&aacute;m)</klr></trd>
  <trd lng="pl">ca&lstroke;kowity</trd>
  <trdgrp lng="ru">
    <trd>&c_c;&c_je;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_c;&c_je;&c_l;&c_o;&c_ch;&c_i;&c_s;&c_l;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>digit&aacute;lny</trd>,
    <trd>&ccaron;&iacute;seln&yacute;</trd>
  </trdgrp>
  <trd lng="sv">heltals-</trd>
  <trdgrp lng="uk">
    <trd>&#x446;&#x456;&#x43B;&#x438;&#x439;</trd>,
    <trd>&#x446;&#x456;&#x43B;&#x43E;&#x447;&#x438;&#x441;&#x43B;&#x43E;&#x432;&#x438;&#x439;</trd>
  </trdgrp>
  <trd lng="zh">&#x6574;&#x6570; <pr>zh&#x11B;ngsh&#xF9;</pr></trd>
</drv>

<drv mrk="entjer.gauxsa0o">
  <kap>ga&ubreve;sa <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="komple.0o.MAT">Komplekso</ref>, kies
      entjera kaj imaginara partoj estas entjeroj.
    </dif>
    <ref tip="vid" cel="gauxs.0o">Ga&ubreve;so.</ref>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_w;&c_s;&c_a;&c_w; <ind>&c_l;&c_ib;&c_k;</ind></trd>
  <trd lng="de">ganze Gau&szlig;sche <ind>Zahl</ind></trd>
  <trd lng="en">gaussian <ind>integer</ind></trd>
  <trd lng="fr">entier de Gauss</trd>
  <trd lng="hu">Gauss-eg&eacute;sz</trd>
  <trd lng="pl">liczba Gaussa</trd>
  <trd lng="ru">&c_g;&c_a;&c_u;&c_s;&c_s;&c_o;&c_v;&c_o;
<ind>&c_ch;&c_i;&c_s;&c_l;&c_o;</ind></trd>
</drv>

<drv mrk="entjer.natura0o">
  <kap>natura <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 7</lok></fnt>
  <snc>
    <dif>
      <ref cel="negati.ne0a">Nenegativa</ref>
      <ref tip="super" cel="entjer.0o"><tld/>o</ref>
      <klr>(t.e. <frm am="0,1,2,3,...">0, 1, 2, 3,...</frm>)</klr>:
      <ekz>
        la aron de naturaj <tld/>oj oni signas per
        <frm am="NN"><g>N</g></frm>, kaj
        tiun de strikte pozitivaj <tld/>oj per
        <frm am="NN&quot;*&quot;"><g>N</g><sup>*</sup></frm>.
      </ekz>
    </dif>
    <ref tip="sin" cel="nombr.natura0o">natura nombro.</ref>
    <rim>
      En elementa aritmetiko, oni ofte lasas la nombron <frm>0</frm>
      ekster
      la aro de naturaj <tld/>oj.
    </rim>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_t;&c_u;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;
<ind>&c_l;&c_ib;&c_k;</ind></trd>
  <trdgrp lng="cs">
    <trd>cel&eacute; kladn&eacute; &ccaron;&iacute;slo</trd>,
    <trd>p&rcaron;irozen&eacute; &ccaron;&iacute;slo</trd>
  </trdgrp>
  <trd lng="de">nat&uuml;rliche <ind>Zahl</ind></trd>
  <trd lng="en">natural <ind>number</ind></trd>
  <trd lng="fr">entier naturel</trd>
  <trd lng="he">&#x5DE;&#x5E1;&#x5E4;&#x5E8; &#x5E9;&#x5DC;&#x5DD; &#x5D8;&#x5D1;&#x5E2;&#x5D9;</trd>
  <trd lng="hu">term&eacute;szetes <ind>sz&aacute;m</ind></trd>
  <trd lng="pl">liczba naturalna</trd>
  <trd lng="ru">&c_n;&c_a;&c_t;&c_u;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_je;
<ind>&c_ch;&c_i;&c_s;&c_l;&c_o;</ind></trd>
  <trdgrp lng="sk">
    <trd>cel&eacute; kladn&eacute; &ccaron;&iacute;slo</trd>,
    <trd>prirodzen&eacute; &ccaron;&iacute;slo</trd>
  </trdgrp>
  <trd lng="sv">naturligt tal</trd>
  <trdgrp lng="zh">
    <trd>&#x96CD;&#x5BB9; <pr>y&#x14D;ngr&#xF3;ng</pr></trd>,
    <trd>&#x81EA;&#x7136;&#x6570; <pr>z&#xEC;r&#xE1;nsh&#xF9;</pr></trd>
  </trdgrp>
</drv>
</art>
