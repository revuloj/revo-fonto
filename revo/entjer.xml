<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>entjer</rad>/o
</kap>
<drv mrk="entjer.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 7</lok></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="nombr.0o.MAT">Nombro</ref> el la sinsekvo 0, 1, 
      2, 3... a&ubreve; el la sinsekvo -1, -2, -3...:
      <ekz>
        la aron de <tld/>oj oni signas per <em>Z</em>.
      </ekz>
    </dif>
    <ref tip="super" cel="racio5.0o">raciono;</ref>
    <refgrp tip="vid">
      <ref cel="aritme.0o">aritmetiko</ref>,
      <ref cel="kardin.0o.MAT">kardinalo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">ganze <ind>Zahl</ind></trd>
  <trd lng="en">integer</trd>
  <trdgrp lng="fr">
    <trd>entier<klr tip="ind"> (nombre)</klr></trd>,
    <trd>nombre entier</trd>,
    <trd>entier relatif</trd>,
    <trd>entier rationnel</trd>
  </trdgrp>
  <trd lng="ru">&c_c;&c_je;&c_l;&c_o;&c_je; <ind>&c_ch;&c_i;&c_s;&c_l;&c_o;</ind></trd>
</drv>

<drv mrk="entjer.natura0o">
  <kap>natura <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 7</lok></fnt>
  <snc>
    <dif>
      <ref cel="negati.ne0a">Nenegativa</ref> 
      <ref tip="super" cel="entjer.0o"><tld/>o</ref> (t.e. 0, 1, 2, 3...):
      <ekz>
        la aron de naturaj <tld/>oj oni signas per <em>N</em>,
        kaj tiun de strikte pozitivaj <tld/>oj per <em>N</em><sup>*</sup>.
      </ekz>
    </dif>
    <rim>
      En elementa aritmetiko, oni ofte lasas la nombron 0 ekster
      la aro de naturaj <tld/>oj.
    </rim>
  </snc>
  <trd lng="de">nat&uuml;rliche <ind>Zahl</ind></trd>
  <trd lng="en">natural <ind>number</ind></trd>
  <trd lng="fr">entier naturel</trd>
  <trd lng="ru">&c_n;&c_a;&c_t;&c_u;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_je; <ind>&c_ch;&c_i;&c_s;&c_l;&c_o;</ind></trd>
</drv>
</art>
