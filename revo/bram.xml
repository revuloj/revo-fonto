<art mrk="$Id: bram.xml,v 1.24 2023/08/31 08:48:53 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bram</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="bram.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bram.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda <ref tip="lst" lst="voko:fi&scirc;oj" cel="fisx.0o">fi&scirc;o</ref>
      grupe vivanta en malrapidaj riveroj de E&ubreve;ropo; 
      <ref tip="lst" lst="voko:zoologiaj_specioj" cel="speci.0o">specio</ref>
      el familio <ref tip="malprt" cel="ciprin1.0oj">ciprinedoj</ref>
      (<trd lng="la">Abramis brama</trd>):
      <ekz>
        gracia <tld/>o kun longaj bu&scirc;haroj
        <fnt>
          <aut>B. N&ecaron;mcov&aacute;, trad. V. Tobek kaj K.
          Proch&aacute;zka</aut>,
          <vrk><url ref="https://eo.wikisource.org/wiki/Avineto/VII.">
          Avineto</url></vrk>,
          <lok>[sen dato]</lok>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve;longe de la rivero oni elprenis plurmil tunojn da mortintaj
        fi&scirc;oj (plejparte &ccirc;inaj karpoj, <tld/>oj, siluroj, sandroj, karpoj)
        <fnt><aut>Katalin Smid&eacute;liusz</aut>: 
          <vrk><url ref="&Monato;/2000/006912.php">Cinike cianide -- &ccirc;u respondecas
          Okcidento?</url></vrk>,
          <bib>Monato</bib>
          <lok>2000/04, p. 15</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/d/d2/Abramis_brama_by_RpM.JPG/640px-Abramis_brama_by_RpM.JPG" prm="CC BY-SA 3.0"><tld/>o (Abramis brama)
        <fnt>
          <aut>Reaperman, propra verko</aut>
          <lok><url ref="&WCW;/File:Abramis_brama_by_RpM.JPG">&WCW;/File:Abramis_brama_by_RpM.JPG</url></lok>
        </fnt>
    </bld>
    <trd lng="be">&c_l;&c_je;&c_sh;&c_ch;</trd>
    <trdgrp lng="br">
      <trd>brem</trd>,
      <trd>brell</trd>
    </trdgrp>
    <trd lng="de">Blei</trd>
    <trd lng="en">bream</trd>
    <trd lng="fr">br&egrave;me</trd>
    <trd lng="hu">d&eacute;v&eacute;rkeszeg</trd>
    <trd lng="nl">brasem</trd>
    <trd lng="pl">leszcz</trd>
    <trd lng="pt">brema</trd>
    <trd lng="ru">&c_l;&c_je;&c_shch;</trd>
    <trd lng="sv">braxen</trd>
  </snc>
  <rim mrk="bram.0o.RIM_HOM">
    <ref tip="hom" cel="bram1.0o">Bramo</ref> anka&ubreve;
    nomas parton de masto.
  </rim>
  <trd lng="ja">&#x30C8;&#x30B2;&#x30EB;&#x30F3;&#x30DE;&#x30B9;&#x30C8;</trd>
</drv>
</art>
