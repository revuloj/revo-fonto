<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>bram</rad>/o
</kap>
<drv mrk="bram.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Specio el familio <ref tip="super" cel="ciprin1.0oj">ciprinedoj</ref>
      (<trd lng="la">Abramis brama</trd>), granda riverfi&scirc;o. 
    </dif>
    <trd lng="be">&c_l;&c_je;&c_sh;&c_ch;</trd>
    <trdgrp lng="br">
      <trd>brem</trd>,
      <trd>brell</trd>
    </trdgrp>
    <trd lng="de">Blei</trd>
    <trd lng="en">bream</trd>
    <trd lng="fr">br&egrave;me</trd>
    <trd lng="hu">d&eacute;v&eacute;rkeszeg</trd>
    <trd lng="ru">&c_l;&c_je;&c_shch;</trd>
    <trd lng="sv">braxen</trd>
  </snc>
  <snc num="2">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Tria, plej supera parto de masto.  
    </dif>
    <trd lng="be">&c_b;&c_r;&c_a;&c_m;</trd>
    <trd lng="en">topgallant</trd>
    <trd lng="fr">ton<klr tip="ind"> (mar.)</klr></trd>
    <trd lng="hu">&aacute;rbocsud&aacute;r</trd>
    <trd lng="ru">&c_b;&c_r;&c_a;&c_m;</trd>
  </snc>
</drv>
</art>
