<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>klu&ccirc;</rad>/i
</kap>
<drv mrk="klucx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">TEK</uzo>
  <uzo tip="fak">AUT</uzo>
  <snc>
    <dif>
      Kunigi per iu mekanismo la motoron de ma&scirc;ino kaj la organon,
      kiujn &gcirc;i devas movi.
    </dif>
  </snc>
  <trd lng="nl">koppelen</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_t;&c_mol; &c_t;&c_r;&c_a;&c_n;&c_s;&c_m;&c_i;&c_s;&c_s;&c_i;&c_ju;</trd>,
    <trd>&c_v;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_t;&c_mol; &c_s;&c_c;&c_je;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
<drv mrk="klucx.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">TEK</uzo>
  <uzo tip="fak">AUT</uzo>
  <snc>
    <dif>
      Mekanismo servanta por <tld/>i:
      <ekz>
        la <tld/>iloj de la a&ubreve;toj estas a&ubreve; konusaj
        a&ubreve; unudiskaj a&ubreve; plurdiskaj; en aliaj ma&scirc;inoj ili povas
        esti mufaj a&ubreve; dentaj.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">koppeling</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_m;&c_i;&c_s;&c_s;&c_i;&c_ja;</trd>,
    <trd>&c_s;&c_c;&c_je;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je; <klr>(&c_v; &c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;&c_i;&c_l;&c_je;)</klr></trd>
  </trdgrp>
</drv>
<drv mrk="klucx.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Ado <tld/>i.
    </dif>
  </snc>
<trd lng="nl">het <ind>koppelen</ind></trd>
</drv>
<drv mrk="klucx.0opedalo">
  <kap><tld/>opedalo</kap>
  <uzo tip="fak">AUT</uzo>
  <snc>
    <dif>
      Pedalo, servanta por <tld/>i la motoron de a&ubreve;to kun la radoj.
    </dif>
  </snc>
  <trd lng="nl">koppelpedaal</trd>
  <trd lng="ru">&c_p;&c_je;&c_d;&c_a;&c_l;&c_mol; &c_s;&c_c;&c_je;&c_p;&c_l;&c_je;&c_n;&c_i;&c_ja;</trd>
</drv>
<drv mrk="klucx.0risorto">
  <kap><tld/>orisorto</kap>
  <uzo tip="fak">AUT</uzo>
  <snc>
    <dif>
      Risorto, kiu fikstenas <tld/>on a&ubreve; mal<tld/>on.
    </dif>
  </snc>
<trd lng="nl">koppelveer</trd>
</drv>
<drv mrk="klucx.0stango">
  <kap><tld/>stango</kap>
  <uzo tip="fak">AUT</uzo>
  <snc>
    <dif>
      Stango, per kiu oni estigas <tld/>on a&ubreve; mal<tld/>on.
    </dif>
  </snc>
  <trd lng="nl">koppelstang</trd>
  <trd lng="ru">&c_r;&c_y;&c_ch;&c_a;&c_g; &c_s;&c_c;&c_je;&c_p;&c_l;&c_je;&c_n;&c_i;&c_ja;</trd>
</drv>
</art>

