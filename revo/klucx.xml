<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: klucx.xml,v 1.22 2024/09/12 09:29:59 revo Exp $">
<kap>
   <rad>klu&ccirc;</rad>/i
   <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="klucx.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <uzo tip="fak">TEK</uzo>
   <uzo tip="fak">AUT</uzo>
   <snc>
     <dif>
       Kunigi per iu mekanismo la motoron de ma&scirc;ino kaj la
organojn,
       kiujn &gcirc;i devas movi.
     </dif>
     <refgrp tip="vid">
       <ref cel="konekt.0i">konekti</ref>,
       <ref cel="kupl.0i">kupli</ref>
     </refgrp>
   </snc>
   <trd lng="be">&c_s;&c_ch;&c_a;&c_p;&c_l;&c_ja;&c_c;&c_mol; <klr>(&c_a;&c_b; &c_t;&c_r;&c_a;&c_n;&c_s;&c_m;&c_ib;&c_s;&c_ib;&c_ib;)</klr></trd>
   <trdgrp lng="de">
     <trd>kuppeln</trd>,
     <trd>koppeln</trd>,
     <trd>einkuppeln</trd>,
     <trd>verkuppeln</trd>
   </trdgrp>
   <trd lng="en">clutch</trd>
   <trd lng="es">embragar</trd>
   <trdgrp lng="hu">
     <trd>&ouml;sszekapcsol</trd>,
     <trd>tengelykapcsol&oacute;t bekapcsol</trd>,
     <trd>kuplungoz</trd>
   </trdgrp>
   <trdgrp lng="it">
     <trd>innestare<klr> (mec.)</klr></trd>,
     <trd>ingranare</trd>
   </trdgrp>
   <trd lng="ja">&#x30AF;&#x30E9;&#x30C3;&#x30C1;&#12434;&#20837;&#x308C;&#x308B; 
   <pr>&#x30AF;&#x30E9;&#x30C3;&#x30C1;&#12434;&#12356;&#x308C;&#x308B;</pr></trd>
  <trd lng="nl">koppelen</trd>
   <trd lng="fr">embrayer</trd>
   <trd lng="pl">sprz&eogonek;ga&cacute;</trd>
   <trdgrp lng="ru">
     <trd>
     &c_i;&c_s;&c_p;&c_o;&c_l;&c_mol;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;
     <ind>&c_t;&c_r;&c_a;&c_n;&c_s;&c_m;&c_i;&c_s;&c_s;&c_i;&c_ju;</ind>
     </trd>,
     <trd>
     &c_i;&c_s;&c_p;&c_o;&c_l;&c_mol;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;
     <ind>&c_s;&c_c;&c_je;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je;</ind>
     </trd>
     <trd>&c_s;&c_c;&c_je;&c_p;&c_l;&c_ja;&c_t;&c_mol;<klr tip="amb">
     (&c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;.)</klr></trd>
   </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_ch;&c_ib;&c_p;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&#8217;&c_jeu;&c_d;&c_n;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_m;&c_i;&c_k;&c_a;&c_t;&c_i;/&c_v;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_t;&c_i; &c_p;&c_je;&c_r;&c_je;&c_d;&c_a;&c_ch;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21512;&#x4E0A;&#x79BB;&#21512;&#22120; <pr>h&#xE9;sh&#xE0;ngl&#xED;h&#xE9;q&#xEC;</pr></trd>,
    <trd>&#x7D50;&#21512;&#x96E2;&#21512;&#22120; <pr>ji&#xE9;h&#xE9;l&#xED;h&#xE9;q&#xEC;</pr></trd>,
    <trd>&#x7D50;&#21512;&#x96E2;&#21512;&#22120;&#21512; <pr>ji&#xE9;h&#xE9;l&#xED;h&#xE9;q&#xEC;h&#xE9;</pr></trd>,
    <trd>&#32806;&#21512; <pr>&#x1D2;uh&#xE9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="klucx.0ado">
   <kap><tld/>ado</kap>
   <snc>
     <dif>
       Ago <tld/>i.
     </dif>
   </snc>
   <trd lng="be">&c_s;&c_ch;&c_a;&c_p;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je; <klr>(&c_d;&c_z;&c_je;&c_ja;&c_n;&c_mol;&c_n;&c_je;)</klr></trd>
   <trd lng="de">Kuppeln</trd>
   <trd lng="fr">embrayage<klr tip="ind"> (action)</klr></trd>
   <trdgrp lng="hu">
     <trd>tengelykapcsol&oacute;s</trd>,
     <trd>kuplungoz&aacute;s</trd>
   </trdgrp>
   <trdgrp lng="it">
     <trd>innestare<klr> (mec.)</klr></trd>,
     <trd>ingranare</trd>
   </trdgrp>
   <trd lng="ja">&#x30AF;&#x30E9;&#x30C3;&#x30C1;&#x64CD;&#x4F5C; 
   <pr>&#x30AF;&#x30E9;&#x30C3;&#x30C1;&#x305D;&#12358;&#12373;</pr></trd>
  <trd lng="nl">het <ind>koppelen</ind></trd>
   <trdgrp lng="pl">
     <trd>sprz&eogonek;&zdot;enia</trd>,
     <trd>sprz&eogonek;ganie</trd>
   </trdgrp>
   <trd lng="ru">&c_s;&c_c;&c_je;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je;
   <klr tip="amb"> (&c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;.)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_ch;&c_je;&c_p;&c_l;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_z;&#8217;&c_jeu;&c_d;&c_n;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_v;&c_m;&c_i;&c_k;&c_a;&c_n;&c_n;&c_ja;/&c_v;&c_k;&c_l;&c_ju;&c_ch;&c_je;&c_n;&c_n;&c_ja;
    &c_p;&c_je;&c_r;&c_je;&c_d;&c_a;&c_ch;&c_ib;</trd>
  </trdgrp>
</drv>

<drv mrk="klucx.0ilo">
   <kap><tld/>ilo <fnt><bib>PIV1</bib></fnt></kap>
   <uzo tip="fak">TEK</uzo>
   <uzo tip="fak">AUT</uzo>
   <snc>
     <dif>
       Mekanismo servanta por <tld/>i:
       <ekz>
         la <tld/>iloj de la a&ubreve;toj estas a&ubreve; konusaj
         a&ubreve; unudiskaj a&ubreve; plurdiskaj, en aliaj ma&scirc;inoj
         ili povas esti mufaj a&ubreve; dentaj;
       </ekz>
      <ekz>
        <nom>Karl Benz</nom> <klr>[&#8230;]</klr> inventis plurajn erojn por a&ubreve;tomobilo, ekz.
        <klr>[&#8230;]</klr> la klu&ccirc;ilon, la karburilon kaj la rapidum&scirc;an&gcirc;ilon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=15204">Karl Benz</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kupl.0ilo">kuplilo</ref>
  </snc>
   <trdgrp lng="be">
     <trd>&c_s;&c_ch;&c_a;&c_p;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je; <klr>(&c_p;&c_r;&c_y;&c_l;&c_a;&c_d;&c_a;)</klr></trd>,
     <trd>&c_m;&c_u;&c_f;&c_t;&c_a; <ind>&c_s;&c_ch;&c_a;&c_p;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_ja;</ind></trd>
   </trdgrp>
   <trd lng="de">Kupplung</trd>
   <trd lng="en">clutch</trd>
   <trd lng="es">embrague</trd>
   <trdgrp lng="hu">
     <trd>tengelykapcsol&oacute;</trd>,
     <trd>kuplung</trd>
   </trdgrp>
   <trdgrp lng="it">
     <trd>frizione</trd>,
     <trd>innesto<klr> (mec.)</klr></trd>
   </trdgrp>
   <trd lng="ja">&#x30AF;&#x30E9;&#x30C3;&#x30C1;</trd>
  <trd lng="nl">koppeling</trd>
   <trd lng="fr">embrayage<klr tip="ind"> (m&eacute;canisme)</klr></trd>
   <trd lng="pl">sprz&eogonek;g&lstroke;o</trd>
   <trdgrp lng="ru">
     <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_m;&c_i;&c_s;&c_s;&c_i;&c_ja;</trd>,
     <trd>&c_s;&c_c;&c_je;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je;
     <klr tip="amb"> (&c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;.)</klr></trd>
   </trdgrp>
  <trd lng="uk">&c_z;&c_ch;&c_je;&c_p;&c_l;&c_je;&c_n;&c_n;&c_ja; (&c_m;&c_je;&c_h;&c_a;&c_n;&c_ib;&c_z;&c_m;)</trd>
  <trdgrp lng="zh">
    <trd>&#x79BB;&#21512;&#22120; <pr>l&#xED;h&#xE9;q&#xEC;</pr></trd>,
    <trd>&#x8FDE;&#x63A5;&#22120; <pr>li&#xE1;nji&emacron;q&#xEC;</pr></trd>,
    <trd>&#x7D50;&#21512;&#22120; <pr>ji&#xE9;h&#xE9;q&#xEC;</pr></trd>,
    <trd>&#32852;&#x63A5; <pr>li&#xE1;nji&emacron;</pr></trd>,
    <trd>&#32852;&#x7ED3; <pr>li&#xE1;nji&#xE9;</pr></trd>,
    <trd>&#32852;&#x8F74;&#33410; <pr>li&#xE1;nzh&#xF3;uji&#xE9;</pr></trd>,
    <trd>&#x8FDE;&#x7ED3;&#22120; <pr>li&#xE1;nji&#xE9;q&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="klucx.0opedalo">
   <kap><tld/>opedalo <fnt><bib>PIV1</bib></fnt></kap>
   <uzo tip="fak">AUT</uzo>
   <snc>
     <dif>
       Pedalo, servanta por <tld/>i la motoron de a&ubreve;to kun la
       radoj.
     </dif>
   </snc>
   <trd lng="be">&c_p;&c_e;&c_d;&c_a;&c_l;&c_mol; <ind>&c_s;&c_ch;&c_a;&c_p;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_ja;</ind></trd>
   <trd lng="de">Kupplungspedal</trd>
   <trd lng="en">clutch pedal</trd>
   <trd lng="ja">&#x30AF;&#x30E9;&#x30C3;&#x30C1;&#x30DA;&#x30C0;&#x30EB;</trd>
  <trd lng="nl">koppelpedaal</trd>
   <trd lng="fr">p&eacute;dale d'<ind>embrayage</ind></trd>
   <trdgrp lng="hu">
     <trd>tengelykapcsol&oacute;-ped&aacute;l</trd>,
     <trd>kuplungped&aacute;l</trd>
   </trdgrp>
   <trdgrp lng="it">
     <trd>pedale della <ind>frizione</ind></trd>
   </trdgrp>
   <trdgrp lng="pl">
     <trd>peda&lstroke; <ind>sprz&eogonek;g&lstroke;a</ind></trd>,
     <trd>sprz&eogonek;g&lstroke;o<klr> (pot.)</klr></trd>
   </trdgrp>
   <trd lng="ru">&c_p;&c_je;&c_d;&c_a;&c_l;&c_mol;
   &c_s;&c_c;&c_je;&c_p;&c_l;&c_je;&c_n;&c_i;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#x79BB;&#21512;&#22120;&#x8E0F;&#x677F; <pr>l&#xED;h&#xE9;q&#xEC;t&#xE0;b&#x1CE;n</pr></trd>,
    <trd>&#x79BB;&#21512;&#x677F; <pr>l&#xED;h&#xE9;b&#x1CE;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="klucx.0risorto">
   <kap><tld/>orisorto <fnt><bib>PIV1</bib></fnt></kap>
   <uzo tip="fak">AUT</uzo>
   <snc>
     <dif>
       Risorto, kiu fikstenas <tld/>on a&ubreve; mal<tld/>on.
     </dif>
   </snc>
   <adm>
     La difino uzas nedifinitajn terminojn ~o kaj mal~o (stato, ago?).
     Ankaux ne klaras, kiel oni povas ilin "fiksteni".
     <aut>MB</aut>
   </adm>
   <trd lng="be">&c_z;&c_v;&c_a;&c_r;&c_o;&c_t;&c_n;&c_a;&c_ja; <ind>&c_s;&c_p;&c_r;&c_u;&c_zh;&c_y;&c_n;&c_a;</ind> <klr>(&c_s;&c_ch;&c_a;&c_p;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>
   <trd lng="de">Kupplungsfeder</trd>
   <trd lng="en">clutch return spring</trd>
   <trdgrp lng="fr">
     <trd>diaphragme d'<ind>embrayage</ind></trd>,
     <trd>ressort d'<ind>embrayage</ind></trd>
   </trdgrp>
   <trd lng="hu">tengelykapcsol&oacute;-nyom&oacute;rug&oacute;</trd>
   <trdgrp lng="it">
     <trd>molla della <ind>frizione</ind></trd>
   </trdgrp>
   <trd lng="nl">koppelveer</trd>
</drv>

<drv mrk="klucx.0stango">
   <kap><tld/>stango <fnt><bib>PIV1</bib></fnt></kap>
   <uzo tip="fak">AUT</uzo>
   <snc>
     <dif>
       Stango, per kiu oni estigas <tld/>on a&ubreve; mal<tld/>on.
     </dif>
     <ref tip="vid" cel="rapid.0umo">rapidumo</ref>
   </snc>
   <adm>
     PIV2 nomas tion "rapidum-&scirc;an&gcirc;ilo".
     <aut>MB</aut>
   </adm>
   <trdgrp lng="be">
     <trd>&c_v;&c_a;&c_g;&c_a;&c_r; <ind>&c_s;&c_ch;&c_a;&c_p;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_ja;</ind></trd>,
     <trd>&c_r;&c_y;&c_ch;&c_a;&c_g; <ind>&c_s;&c_ch;&c_a;&c_p;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_ja;</ind></trd>
   </trdgrp>
   <trd lng="de">Kuppelstange</trd>
   <trdgrp lng="en">
     <trd>clutch arm</trd>,
     <trd>clutch lever</trd>
   </trdgrp>
   <trd lng="ja">&#x30AF;&#x30E9;&#x30C3;&#x30C1;&#x30EC;&#x30D0;&#x30FC;</trd>
  <trd lng="nl">koppelstang</trd>
   <trd lng="fr">levier d'<ind>embrayage</ind></trd>
   <trd lng="hu">tengelykapcsol&oacute;kar</trd>
   <trdgrp lng="it">
     <trd>leva della <ind>frizione</ind></trd>
   </trdgrp>
   <trdgrp lng="pl">
     <trd>d&zacute;wignia <ind>sprz&eogonek;g&lstroke;a</ind></trd>
   </trdgrp>
   <trd lng="ru">
   &c_r;&c_y;&c_ch;&c_a;&c_g;
   &c_s;&c_c;&c_je;&c_p;&c_l;&c_je;&c_n;&c_i;&c_ja;
   </trd>
</drv>
</art>
