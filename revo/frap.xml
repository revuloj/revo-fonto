    <trd>&c_u;&c_d;&c_a;&c_r;</trd>,
    <trd>&c_u;&c_d;&c_a;&c_r;&c_n;&c_i;&c_j; <klr>(&c_z;&c_v;&c_u;&c_k;)</klr></trd>
  </trdgrp>
