  </trdgrp>
  <trd lng="nl">klapperen</trd>
  <trdgrp lng="pl">
    <trd>zderza&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_o;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_u;&c_d;&c_a;&c_r;&c_i;&c_t;&c_mol;&c_s;&c_ja;
    &c_d;&c_r;&c_u;&c_g; &c_o; &c_d;&c_r;&c_u;&c_g;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_u;&c_k;&c_n;&c_u;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_ib;&c_t;&c_k;&c_n;&c_u;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_u;&c_d;&c_a;&c_r;&c_i;&c_t;&c_i;&c_s;&c_ja; &c_o;&c_d;&c_i;&c_n; &c_o;&c_b; &c_o;&c_d;&c_n;&c_o;&c_g;&c_o;</trd>,
    <trd>&c_k;&c_l;&c_a;&c_c;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_t;&c_u;&c_k;&c_o;&c_t;&c_ib;&c_t;&c_i; <klr>(&c_p;&c_r;&c_o; &c_z;&c_u;&c_b;&c_i;)</klr></trd>
