<drv mrk="frap.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="frap.0ado.KOMUNE">
    <dif>
      Ago <tld/>i:
      <ekz>
        almozula <tld/>ado &ccirc;e multaj pordoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        ali&gcirc;is al ili &ccirc;iam pli la&ubreve;ta <tld/>ado de homaj piedoj, kiuj kuris sur la &scirc;tonoj
        de la trotuaro
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        de malproksime <klr>[a&ubreve;di&gcirc;is]</klr> pied<tld/>ado de &ccirc;evaloj
        <fnt>
          <bib>Fab3</bib>
        </fnt>;
      </ekz>
      <ekz>
        muziko ne konsistas en sola <tld/>ado de klavoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        eksonis fluto kaj <tld/>ado de l' pokaloj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        inter<tld/>ado de la glasoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="bg">
