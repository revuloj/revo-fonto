<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>glikoz</rad>/o
</kap>
<drv mrk="glikoz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Sukera substanco trovi&gcirc;anta en vinbero, amela&jcirc;o ktp. &Gcirc;ia kemika kompona&jcirc;o estas C<sub>6</sub>H<sub>12</sub>O<sub>6</sub>. &Gcirc;i estas senkolora.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_l;&c_ju;&c_k;&c_o;&c_z;&c_a;</trd>
  <trdgrp lng="de">
     <trd>Glukose</trd>,
     <trd>Glykose</trd>,
     <trd>Traubenzucker</trd></trdgrp>
  <trd lng="fr">glucose</trd>
  <trdgrp lng="nl">
    <trd>glucose</trd>,
    <trd>dextrose</trd>,
    <trd>druivensuiker</trd>
  </trdgrp>
  <trd lng="pt">glicose</trd>
  <trd lng="ru">&c_g;&c_l;&c_ju;&c_k;&c_o;&c_z;&c_a;</trd>
</drv>
</art>
