<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: glikoz.xml,v 1.20 2022/04/30 08:42:40 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>glikoz</rad>/o <fnt><bib>PV</bib></fnt>,
  <var><kap><ofc>8</ofc>
    <rad>glukoz</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>
<drv mrk="glikoz.0o">
  <kap><tld/>o, <var><kap>glukozo</kap></var></kap>
  <snc mrk="glikoz.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Sukera senkolora <ref tip="super" cel="substa.0o.FIZ">substanco</ref>
      trovi&gcirc;anta en <ref tip="malprt" cel="ber.vin0o">vinbero</ref>, amela&jcirc;o kaj aliaj 
      (<frm>C<sub>6</sub>H<sub>12</sub>O<sub>6</sub></frm>):
      <ekz>
        la sangosero enhavas <klr>[&#x2026;]</klr> proteinojn, ne&ubreve;trajn 
        grasojn, <tld/>on
        <fnt>
          <aut>J. Hradil</aut>,
          <vrk><url ref="http://www.freeweb.hu/eventoj/steb/medicino/med-mallonge.htm">
          La &ccirc;elo kaj la histoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la pli vaste konataj sukeroj, glukozo kaj fruktozo, havas en &ccirc;iu molekulo ses
        karbon-atomojn, ribozo havas nur kvin
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/007157.html">Vivo: Riboj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <ind>siropo de glukozo</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2013/010973.php">Bombo-bankroto</url></vrk>
          <lok>2013</lok>
        </fnt>;
        <trd lng="de">Glukosesirup</trd>
      </ekz>
      <ekz>
        <ind>nivelo de glukozo</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2015/011367.php">Noviga apliko je dispono de kuracistoj kaj pacientoj</url></vrk>
          <lok>2015</lok>
        </fnt>;
        <trd lng="de">Glukosespiegel</trd>
      </ekz>
      <ekz>
        la korpo nur uzis a&ubreve; grason a&ubreve; proteinon pro manko de glukozo 
        en la &ccirc;elo, kaj la paciento maldiki&gcirc;as
        <fnt>
          <aut>I. Cris&oacute;gno</aut>,
          <vrk><url ref="http://www.geocities.com/HotSprings/4234/diabeto.html?200824">
          Diabeto</url></vrk>,
          <lok>[1996?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_l;&c_ju;&c_k;&c_o;&c_z;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>glucosa</trd>,
    <trd>glicosa</trd>,
    <trd>dextrosa</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>gluk&oacute;za</trd>,
    <trd>hroznov&yacute; cukr</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Glukose</trd>,
    <trd>Glykose</trd>,
    <trd>Traubenzucker</trd>
  </trdgrp>
  <trd lng="en">glucose</trd>
  <trd lng="fr">glucose</trd>
  <trdgrp lng="nl">
    <trd>glucose</trd>,
    <trd>dextrose</trd>,
    <trd>druivensuiker</trd>
  </trdgrp>
  <trd lng="pt">glicose</trd>
  <trd lng="ru">&c_g;&c_l;&c_ju;&c_k;&c_o;&c_z;&c_a;</trd>
  <trd lng="sk">gluk&oacute;za</trd>
  <trd lng="tr">glikoz</trd>
  <trd lng="uk">&#x433;&#x43B;&#x44E;&#x43A;&#x43E;&#x437;&#x430;</trd>
</drv>
</art>
