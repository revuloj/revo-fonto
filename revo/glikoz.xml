<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>glikoz</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="glikoz.0o">
  <kap><tld/>o</kap>
  <snc mrk="glikoz.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Sukera senkolora substanco trovi&gcirc;anta en vinbero, 
      amela&jcirc;o kaj aliaj 
      (C<sub>6</sub>H<sub>12</sub>O<sub>6</sub>):
      <ekz>
        la sangosero enhavas <klr>...</klr> proteinojn, ne&ubreve;trajn 
        grasojn, <tld/>on
        <fnt>
          <aut>J. Hradil</aut>,
          <vrk><url
          ref="http://www.freeweb.hu/eventoj/steb/medicino/med-mallonge.htm">
          La &ccirc;elo kaj la histoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="glukoz.0o">glukozo</ref>
  </snc>
  <trd lng="be">&c_g;&c_l;&c_ju;&c_k;&c_o;&c_z;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>glucosa</trd>,
    <trd>glicosa</trd>,
    <trd>dextrosa</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>glukóza</trd>,
    <trd>hroznový cukr</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Glukose</trd>,
    <trd>Glykose</trd>,
    <trd>Traubenzucker</trd>
  </trdgrp>
  <trd lng="en">glucose</trd>
  <trd lng="fr">glucose</trd>
  <trdgrp lng="nl">
    <trd>glucose</trd>,
    <trd>dextrose</trd>,
    <trd>druivensuiker</trd>
  </trdgrp>
  <trd lng="pt">glicose</trd>
  <trd lng="ru">&c_g;&c_l;&c_ju;&c_k;&c_o;&c_z;&c_a;</trd>
  <trd lng="sk">glukóza</trd>
  <trd lng="tr">glikoz</trd>
</drv>
</art>
