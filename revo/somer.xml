<drv mrk="somer.0a">
  <kap><tld/>a</kap>
  <snc mrk="somer.0a.KAL">
    <dif>
      Okazanta en <tld/>o, per sia etoso simila al <tld/>o:
      <ekz>
        varmeta <tld/>a nokto
        <fnt><bib>Voja&gcirc;Imp</bib> <lok>&ccirc;e Lev Tolstoj en Jasnaja
        Poljana</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_je;&c_t;&c_n;&c_ib;</trd>
  <trd lng="cs">letn&iacute;</trd>
  <trd lng="fr">estival</trd>
  <trd lng="id">estival</trd>
  <trd lng="it">estivo</trd>
  <trd lng="ja">&#x590F;&#x306E; <pr>&#x306A;&#12388;&#x306E;</pr></trd>
  <trd lng="pl">letni</trd>
  <trd lng="sk">letn&yacute;</trd>
  <trd lng="uk">&c_l;&c_ib;&c_t;&c_n;&c_ib;&c_j;</trd>
</drv>

<drv mrk="somer.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Dum <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_l;&c_je;&c_t;&c_k;&c_u;</trd>,
    <trd>&c_l;&c_je;&c_t;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>im Sommer</trd>,
    <trd>sommers</trd>
  </trdgrp>
  <trd lng="ca">a l'<ind>estiu</ind></trd>
  <trd lng="fr">en &eacute;t&eacute;</trd>
  <trd lng="hu">ny&aacute;ron</trd>
  <trd lng="it">in <ind>estate</ind></trd>
  <trd lng="ja">&#x590F;&#x306B; <pr>&#x306A;&#12388;&#x306B;</pr></trd>
  <trd lng="nl">'s <ind>zomers</ind></trd>
  <trd lng="pl">w <ind>lecie</ind></trd>
  <trd lng="ru">&c_l;&c_je;&c_t;&c_o;&c_m;</trd>
