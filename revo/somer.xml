<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>somer</rad>/o
</kap>
<drv mrk="somer.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="somer.0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Plej varma sezono de la jaro, post printempo, anta&ubreve; 
a&ubreve;tuno:
      <ekz>
        en <tld/>o ni veturas per diversaj veturiloj, kaj en vintro ni
        veturas per glitveturilo
        <fnt><bib>F</bib>, <lok>&FE; 34</lok></fnt>;
      </ekz>
      <ekz>
        kiu kolektas dum la <tld/>o, estas
        sa&gcirc;a<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_je;&c_t;&c_a;</trd>
  <trd lng="br">ha&ntilde;v</trd>
  <trd lng="ca">estiu</trd>
  <trd lng="cs">l&eacute;to</trd>
  <trd lng="de">Sommer</trd>
  <trd lng="en">summer</trd>
  <trd lng="fr">&eacute;t&eacute;</trd>
  <trd lng="hu">ny&aacute;r</trd>
  <trd lng="nl">zomer</trd>
  <trd lng="pl">lato</trd>
  <trd lng="ro">var&#x103;</trd>
  <trd lng="ru">&c_l;&c_je;&c_t;&c_o;</trd>
  <trd lng="sv">sommar</trd>
  <trd lng="tr">yaz</trd>
  <trd lng="vo">solatim</trd>
</drv>

<drv mrk="somer.0a">
  <kap><tld/>a</kap>
  <snc mrk="somer.0a.KAL">
    <dif>
      Okazanta en <tld/>o, per sia etoso simila al <tld/>o:
      <ekz>
        varmeta <tld/>a nokto
        <fnt><bib>Voja&gcirc;Imp</bib>, <lok>&ccirc;e Lev Tolstoj en Jasnaja
        Poljana</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">estival</trd>
  <trd lng="pl">letni</trd>
</drv>

<drv mrk="somer.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Dum <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_l;&c_je;&c_t;&c_k;&c_u;</trd>,
    <trd>&c_l;&c_je;&c_t;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>im Sommer</trd>,
    <trd>sommers</trd>
  </trdgrp>
  <trd lng="ca">a l'<ind>estiu</ind></trd>
  <trd lng="fr">en &eacute;t&eacute;</trd>
  <trd lng="hu">ny&aacute;ron</trd>
  <trd lng="nl">'s <ind>zomers</ind></trd>
  <trd lng="pl">w <ind>lecie</ind></trd>
  <trd lng="ru">&c_l;&c_je;&c_t;&c_o;&c_m;</trd>
  <trd lng="sv">p&aring; <ind>sommaren</ind></trd>
  <trd lng="tr">yaz&inodot;n</trd>
  <trd lng="vo">solatimo</trd>
</drv>
</art>
