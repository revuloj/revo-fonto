<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>somer</rad>/o
</kap>
<drv mrk="somer.0o">
<kap><ofc>*</ofc><tld/>o</kap>
<snc>
<dif>
Plej varma sezono de la jaro: <ekz>kiu kolektas dum la <tld/>o, estas sa&gcirc;a<fnt>Z</fnt>.
</ekz> 
</dif>
</snc>
<trd lng="be">&c_l;&c_je;&c_t;&c_a;</trd>
<trd lng="cs">l&eacute;to</trd>
<trd lng="de">Sommer</trd>
<trd lng="en">summer</trd>
<trd lng="nl">zomer</trd>
<trd lng="pl">lato</trd>
<trd lng="ru">&#1083;&#1077;&#1090;&#1086;</trd>
<trd lng="sv">sommar</trd>
<trd lng="tr">yaz</trd>
</drv>
<drv mrk="somer.0e">
<kap><tld/>e</kap>
<snc>
<dif>
Dum <tld/>o.  
</dif>
</snc>
<trdgrp lng="be">
  <trd>&c_w;&c_l;&c_je;&c_t;&c_k;&c_u;</trd>, 
  <trd>&c_l;&c_je;&c_t;&c_a;&c_m;</trd>
</trdgrp>
<trdgrp lng="de">
   <trd>im Sommer</trd>,
   <trd>sommers</trd></trdgrp>
<trd lng="ru">&#1083;&#1077;&#1090;&#1086;&#1084;</trd>
<trd lng="sv">p&aring; <ind>sommaren</ind></trd>
<trd lng="tr">yaz&inodot;n</trd>
</drv>
</art>
