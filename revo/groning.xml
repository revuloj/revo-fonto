<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: groning.xml,v 1.21 2023/10/15 18:38:19 revo Exp $">
<kap>
  <rad>Groning</rad>/o <fnt><bib>Viki</bib></fnt>,
  <var><kap><rad var="groningen">Groningen</rad>/o</kap></var>
</kap>
<drv mrk="groning.0o">
  <kap><tld/>o, <var><kap><tld var="groningen"/>o</kap></var></kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="groning.0o.urb">
    <dif>
      <ref tip="lst" lst="voko:urboj" cel="urb.0o">Urbo</ref> en la
      samnoma nederlanda provinco <ref tip="malprt"
      cel="groning.0o.prov">Groningo</ref> (6&#730;34'E, 53&#730;14'N):
      <ekz>
        anka&ubreve; Hago, Delft, Harlemo, Utre&hcirc;to, <tld var="groningen"/>o kaj Mastri&hcirc;to, &ccirc;iuj havas siajn
        historiajn konstrua&jcirc;ojn, monumentojn, muzeojn, tradiciojn kaj eventojn
        <fnt>
          <vrk><url ref="https://www.esperanto-nederland.nl/esp/nederlando.php">Nederlando, malalta
          lando, altaj ambicioj</url></vrk>
          <lok>retejo de Esperanto Nederland</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;tatministro <nom>Panhuis</nom> dronas apud <tld/>o
        <fnt>
          <vrk>Tutmonda Kroniko</vrk>,
          <bib>Esperanto</bib>,
          <lok>1907:11 (20), p. 3</lok>
        </fnt>;
      </ekz>
      <ekz>
        estis <klr>[&#8230;]</klr> lo&gcirc;antoj el Amsterdamo, Alkmaro, Arnhemo,
        Apeldorno, Horno, <tld/>o
        <fnt>
          <vrk><url
          ref="https://resolver.kb.nl/resolve?urn=MMCODA01:000156457:mpeg21:a0088">Nieuwe
          Apeldoornsche courant</url></vrk>,
          <lok>p. 11, 1932-11-15</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ligon kreis <nom>Fred de Geus</nom>, juna programisto de la malsanulejo en <tld
        var="groningen"/>o, Nederlando
        <fnt>
          <vrk><url ref="&Monato;/1985/monato198506.pdf#page=15">GCA kvarobligas sian
          kapaciton</url></vrk>,
          <bib>Monato</bib>,
          <lok>1985/06, p. 15</lok>
        </fnt>;
      </ekz>
      <ekz>
        la hejmiradon al <tld var="groningen"/>o mi faris per trajnoj kaj flugma&scirc;ino
        <fnt>
          <aut>Wil van Ganswijk-Vlasblom</aut>
          <vrk><url ref="https://www.pingveno.nl/klubo/LVS%20sep%2011.pdf#page=9">
            Ferii per kongresoj</url></vrk>
          <lok>La Verda Stelo, septembro 2011, p. 10a</lok>
        </fnt>;
      </ekz>
      <ekz>
        arta ekspozicio nomata <nom>Parrhesia</nom> kiu okazis en
        <tld/>o &gcirc;is Februaro 2016
        <fnt>
          <aut>F. Gobbo</aut>,
          <vrk><url
          ref="https://www.esperanto-nederland.nl/ned/leerstoel/La-Amsterdama-Katedro-nro-30-31-2016-1-1.pdf">Informade
          ekster la Universitato</url></vrk>,
          <lok>La Amsterdama Katedro, 2016:1-2 (30-31), p. 6</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Bob Venhuizen</nom> (naski&gcirc;is la 9-an de aprilo 1957 en <tld/>o, mortis la 26-an de
        septembro 2020 en Roterdamo) estis nederlanda esperantisto
        <fnt>
          <vrk>Forpasoj</vrk>,
          <bib>Esperanto</bib>,
          <lok>2021:1 (1353), p. 23</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fy">Grins</trd>
    <trd lng="ja">&#12501;&#12525;&#12540;&#12491;&#12531;&#12466;&#12531;
    <pr>&#12405;&#12429;&#12540;&#12395;&#12435;&#12370;&#12435;</pr></trd>
  </snc>
  <snc mrk="groning.0o.prov">
    <dif>
      <ref tip="lst" lst="voko:regionoj" cel="provin.0o">Provinco</ref>
      en <ref tip="malprt" cel="nederl.N0o">Nederlando</ref> kies
      &ccirc;efurbo anka&ubreve; nomi&gcirc;as <ref tip="prt"
      cel="groning.0o.urb">Groningo</ref>:
      <ekz>
        la rivero Reitdiep <klr>(...)</klr> estas grava por la drenado de la
        troa akvo en grandaj parto de la provinco <tld var="groningen"/>o
        kaj la norda parto de Drento
        <fnt>
          <aut>Luit Staghouwer</aut>
          <vrk><url ref="https://www.pingveno.nl/klubo/LVS%20apr%2011.pdf#page=12">
            Pri Reitdiep kaj la kluzo de Schapehals</url></vrk>
          <lok>La Verda Stelo, aprilo 2011, pa&gcirc;o 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malkovro de la plej granda ku&scirc;ejo de natura gaso en la mondo
        en la nederlanda provinco <tld/>o
        <fnt>
          <aut>C. Ventura, trad. V. Lutermano</aut>,
          <vrk><url
          ref="&Mondediplo;article2126.html">&Ccirc;inujo, la
          bovino kaj la lakto</url></vrk>,
          <lok>Le Monde diplomatique, 2014-09</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Jakob Telling</nom> naski&gcirc;is la 25-an de novembro 1896 en
        <nom>Oldehove</nom> (<tld/>o)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=28115">Jakob
          Telling</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fy">Grinsl&acirc;n</trd>
    <trd lng="ja">&#12501;&#12525;&#12540;&#12491;&#12531;&#12466;&#12531;&#24030;
    <pr>&#12405;&#12429;&#12540;&#12395;&#12435;&#12370;&#12435;&#12375;&#12421;&#12358;</pr></trd>
  </snc>
  <trd lng="af">Groningen</trd>
  <trd lng="be">&c_G;&c_r;&c_o;&c_n;&c_ib;&c_n;&c_g;&c_je;&c_n;</trd>
  <trd lng="ca">Groningen</trd>
  <trd lng="de">Groningen</trd>
  <trd lng="el">&Chi;&rho;&omikron_ton;&ny;&jota;&ny;&gamma;&kappa;&epsilon;&ny;</trd>
  <trd lng="en">Groningen</trd>
  <trd lng="es">Groninga</trd>
  <trd lng="fr">Groningue</trd>
  <trd lng="gl">Groninga</trd>
  <trd lng="hu">Groningen</trd>
  <trd lng="it">Groninga</trd>
  <trd lng="nl">Groningen</trd>
  <trd lng="oc">Groninga</trd>
  <trd lng="pl">Groningen</trd>
  <trd lng="pt">Groninga</trd>
  <trd lng="ru">&c_G;&c_r;&c_o;&c_n;&c_i;&c_n;&c_g;&c_je;&c_n;</trd>
</drv>

<drv mrk="groning.g0a">
  <kap><tld lit="g"/>a, <var><kap><tld lit="g" var="groningen"/>a</kap></var></kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        la <tld/>aj poldro-landoj <ctl>Westerkwartier</ctl> kaj
        <ctl>Hunsingo</ctl>
        <fnt>
          <vrk>La plej grandaj pumpiloj en la mondo</vrk>,
          <bib>Esperanto</bib>,
          <lok>1919:11 (231), p. 7</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni memoru la ekkultivadon de la <tld/>aj altaj mar&ccirc;oj
        <fnt>
          <aut>J. Hudig, trad. G. F. Makkink</aut>
          <vrk>Malsano kaj sano de la grundo</vrk>
          <lok>Scienca Revuo, 1954, 5:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i iniciatis ke estu vo&ccirc;legado kaj trovis helpon el la <tld var="groningen"/>a grupo
        <fnt>
          <aut>S. Besenyei-Merger</aut>,
          <vrk>Rob Moerbeek - La provleganto de Esperantujo</vrk>,
          <lok>&leftquot;Kontakto&rightquot;, n-ro 285 (2018:3), p. 17a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_o;&c_n;&c_ib;&c_n;&c_g;&c_je;&c_n;&c_s;&c_k;&c_ib;</trd>
  <trd lng="es">groningu&eacute;s</trd>
  <trdgrp lng="fr">
    <trd>groningois<klr> (adj.)</klr></trd>
    <trd>groningoise<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12501;&#12525;&#12540;&#12491;&#12531;&#12466;&#12531;&#12398;
    <pr>&#12405;&#12429;&#12540;&#12395;&#12435;&#12370;&#12435;&#12398;</pr></trd>,
    <trd>&#12501;&#12525;&#12540;&#12491;&#12531;&#12466;&#12531;&#24030;&#12398;
    <pr>&#12405;&#12429;&#12540;&#12395;&#12435;&#12370;&#12435;&#12375;&#12421;&#12358;&#12398;</pr></trd>
  </trdgrp>
  <trd lng="nl">Gronings</trd>
  <trd lng="pl">groningski</trd>
</drv>
</art>

