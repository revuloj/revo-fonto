<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ebonit.xml,v 1.19 2023/08/31 11:49:26 revo Exp $">
<kap>
  <rad>ebonit</rad>/o
</kap>
<drv mrk="ebonit.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="kauxcx.0o">Ka&ubreve;&ccirc;uko</ref>
      malmoligita per vulkanizo, uzata kiel <ref tip="super" cel="izol.0ilo">izolilo</ref>
      en elektraj aparatoj:
      <ekz>
        du <tld/>aj platoj kovritaj per staniolo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=35166">Nikola Tesla</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        bu&scirc;peco <klr>[de klarneto]</klr>, kiu konsistas el beko, (farita el bukso,
        <tld/>o a&ubreve; vitro), sur kie trovi&gcirc;as an&ccirc;o (el kano, foje el plasto) fiksita de
        &ccirc;irka&ubreve;ligo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=20130">Klarneto</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_b;&c_a;&c_n;&c_ib;&c_t;</trd>
  <trd lng="ca">ebonita</trd>
  <trd lng="cs">ebonit</trd>
  <trdgrp lng="de">
     <trd>Ebonit</trd>,
     <trd>Hartgummi</trd></trdgrp>
  <trd lng="en">ebonite</trd>
  <trd lng="es">ebonita</trd>
  <trd lng="fa">&a_A;&a_b;&a_w;&a_n;&f_y;&a_t;</trd>
  <trd lng="fr">&eacute;bonite</trd>
  <trdgrp lng="hu">
     <trd>ebonit</trd>,
     <trd>kem&eacute;nygumi</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30A8;&#x30DC;&#x30CA;&#x30A4;&#x30C8;</trd>,
    <trd>&#x786C;&#x8CEA;&#x30B4;&#x30E0;  [<ind>&#x3053;&#x3046;&#x3057;&#x3064;&#x30B4;&#x30E0;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">eboniet</trd>
  <trd lng="pl">ebonit</trd>
  <trd lng="pt">ebonite</trd>
  <trd lng="ru">&c_e;&c_b;&c_o;&c_n;&c_i;&c_t;</trd>
  <trd lng="sk">ebonit</trd>
  <trd lng="uk">&#x435;&#x431;&#x43E;&#x43D;&#x456;&#x442;</trd>
</drv>
</art>
