<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: infest.xml,v 1.7 2023/11/19 10:22:13 revo Exp $">
<kap>
  <rad>infest</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="infest.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="infest.0i.BIO">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="klr">(pri viva&jcirc;oj)</uzo>
    <dif>
      <ref tip="super" cel="prospe1.0i">Prosperi</ref> kaj <ref tip="super" cel="util.mal0i">malutili</ref> pro sia abundeco:
      <ekz>
        blatoj <tld/>is la &scirc;ipojn
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 4 (54)</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ili apartenis al la besta gento de trogloditoj,
        kiuj <tld/>as la bordojn de la araba golfo
        <fnt>
          <aut>J. L. Borges, trad. M. Giorgini</aut>,
          <vrk><url ref="http://www.yksi.org/tekst/ald/borges_inm-eo.html">
          La senmortulo</url></vrk>,
          <lok>[2009]</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="infekt.0i">infekti</ref>,
      <ref cel="mult.0igxi">multi&gcirc;i</ref>,
      <ref cel="plag.0i">plagi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>masov&ecaron; napadnout parazity</trd>,
    <trd>postihnout</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>befallen</trd>,
    <trd>bev&ouml;lkern <klr>(befallen)</klr></trd>
  </trdgrp>
  <trd lng="fr">infester</trd>
  <trdgrp lng="sk">
    <trd>masovo napadn&uacute;&tcaron; parazitmi</trd>,
    <trd>postihn&uacute;&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_a;&c_v;&c_o;&c_d;&c_n;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_o;&c_v;&c_n;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_u;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="infest.0o">
  <kap><tld/>o, <var><kap><tld/>ado</kap></var></kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="infest.0o.BIO">
    <uzo tip="fak">AGR</uzo>
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">MED</uzo>
    <dif>
      Plimulti&gcirc;o de malutilaj viva&jcirc;oj:
      <ekz>
        sen sufi&ccirc;a sarkado, la trudplantoj forprenus
        akvon, nutra&jcirc;on, lumon kaj spacon for de la
        rikoltplanto, kaj se la <tld/>ado estas granda,
        eble ne estus rikolto
        <fnt>
          <aut>W. F. Rolt</aut>,
          <vrk>El la Polvo de la Tero</vrk>,
          <lok>1967</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="plag.0o">plago</ref>
    </refgrp>
  </snc>
  <trd lng="cs">napad&aacute;n&iacute; parazity</trd>
  <trd lng="de">Befall</trd>
  <trdgrp lng="fr">
    <trd>infestation</trd>,
    <trd>invasion<klr> (de nuisibles)</klr></trd>
  </trdgrp>
  <trd lng="sk">napadanie parazitmi</trd>
  <trd lng="uk">&c_ib;&c_n;&c_v;&c_a;&c_z;&c_ib;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#x4FB5;&#x5165; <pr>q&#x12B;nr&#xF9;</pr></trd>,
    <trd>&#x4FB5;&#x5165; <pr>q&#x12B;nr&#xF9;</pr></trd>
  </trdgrp>
</drv>
</art>

