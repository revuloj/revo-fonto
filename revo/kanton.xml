<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>kanton</rad>/o
</kap>
<drv mrk="kanton.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Malvasta administra <ref tip="super" cel="terito.0o">teritorio</ref>
      en kelkaj regnoj a&ubreve; federa&jcirc;oj.
    </dif>
    <subsnc>
      <dif>
        En Francio parto de distrikto.
      </dif>
    </subsnc>
    <subsnc>
      <dif>
        En Svisio &ccirc;iu el la regnoj de la konfederacio.
      </dif>
    </subsnc>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_t;&c_o;&c_n;</trd>
  <trd lng="de">Kanton</trd>
  <trd lng="en">canton</trd>
  <trdgrp lng="fr">
    <trd>canton</trd>
  </trdgrp>
  <trd lng="nl">kanton</trd>
  <trd lng="ru">&c_k;&c_a;&c_n;&c_t;&c_o;&c_n;</trd>
</drv>
</art>
