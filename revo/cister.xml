<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cister.xml,v 1.31 2025/04/03 02:00:42 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>cistern</rad>/o <fnt><bib>JED</bib></fnt>
</kap>

<drv mrk="cister.0o">
  <kap><tld/>o</kap>
  <snc mrk="cister.0o.AKV">
    <dif>
      Granda <ref tip="super" cel="kolekt.0ujo">kolektujo</ref> por
      <ref tip="prt" cel="akv.pluv0o">pluvakvo</ref>, ordinare subtera:
      <ekz>
        trinku akvon el via <tld/>o, kaj fluantan el via <ref cel="put.0o">puto</ref>
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 5:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        la akvo en la sankta <tld/>o nigri&gcirc;is, kiel sango
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi renkontis &scirc;in apud &gcirc;ardena <tld/>o, tenantan en la mano &jcirc;us el&scirc;iritan kanon,
        kies tufan finon &scirc;i trempis en la akvo kaj sur&scirc;prucis la iridojn
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        silento interrompata de la <klr>[&#8230;]</klr> pla&ubreve;doj de verdaj serpentoj kun plataj
        kapoj en la <tld/>o
        <fnt>
          <bib>FdO</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> korto entenis iun monstran <tld/>on
        el lado, de kiu plurfoje tage mi devis eltiri akvon
        <fnt>
          <aut>A. LeQuint</aut>,
          <vrk>Infantraviva&jcirc;oj</vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="basen.0o.HOR">baseno<sncref/></ref>,
      <ref cel="kav.fos0o">foskavo</ref>,
      <ref cel="kuv.0o.KOMUNE">kuvo</ref>
    </refgrp>
    <trdgrp lng="ca">
      <trd>cisterna</trd>,
      <trd>dip&ograve;sit</trd>
    </trdgrp>
    <trdgrp lng="cs">
      <trd>cisterna</trd>,
      <trd>k&aacute;&dcaron;</trd>,
      <trd>n&aacute;dr&zcaron;</trd>,
      <trd>rezervo&aacute;r</trd>,
      <trd>z&aacute;sobn&iacute;k</trd>
    </trdgrp>
    <trd lng="de">Zisterne</trd>
    <trd lng="es">cisterna</trd>
    <trd lng="fr">citerne</trd>
    <trd lng="he">&mem;&yod;&kaf;&lamed;</trd>
    <trd lng="hu">ciszterna</trd>
    <trd lng="it">cisterna</trd>
    <trdgrp lng="nl">
      <trd>cisterne</trd>,
      <trd>regenput</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>cysterna</trd>,
      <trd>zbiornik</trd>
    </trdgrp>
    <trd lng="pt">cisterna</trd>
    <trdgrp lng="ru">
      <trd>&c_c;&c_i;&c_s;&c_t;&c_je;&c_r;&c_n;&c_a;</trd>,
      <trd>&c_b;&c_a;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>cisterna</trd>,
      <trd>n&aacute;dr&zcaron;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#27700;&#22612; <pr>shu&#464;t&#462;</pr></trd>,
    <trd>&#33988;&#27700;&#27744; <pr>x&ugrave;shu&#464;ch&iacute;</pr></trd>,
    <trd>&#27744;&#23376; <pr>ch&iacute;zi</pr></trd>,
    <trd>&#22823;&#27700;&#31665; <pr>d&agrave;shu&#464;xi&amacron;ng</pr></trd>,
    <trd>&#22823;&#36015;&#27700;&#22120; <pr>d&agrave;izh&#468;shu&#464;q&igrave;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="cister.0o.NAF">
    <dif>
      <ref tip="super" cel="rezerv.0ujo">Rezervujo</ref> por akvo a&ubreve; alia flua&jcirc;o:
      <ekz>
        estis anka&ubreve; du malgrandaj <tld/>oj en unu el la kajutoj, unu por lavakvo kaj la
        alia por trinkakvo
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la ekstrema varmo estis agrabla sperto surstrate a&ccirc;eti el granda <tld/>o
        veran kvason
        <fnt>
          <bib>LOdE</bib>
          <vrk><url ref="&Tekstaro;?nomo=ondo-de-esperanto">Postforumaj
          impresoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">k&odblac;olajtart&aacute;ly</trd>
    <trd lng="de">Tank</trd>
    <trd lng="nl">tank</trd>
    <trdgrp lng="pl">
      <trd>cysterna</trd>,
      <trd>zbiornik</trd>,
      <trd>bak</trd>
    </trdgrp>
  </snc>
    <trdgrp lng="be">
      <trd>&c_c;&c_y;&c_s;&c_t;&c_e;&c_r;&c_n;&c_a;</trd>,
      <trd>&c_b;&c_a;&c_k;</trd>,
      <trd>&c_t;&c_a;&c_n;&c_k; <klr>(&c_d;&c_l;&c_ja; &c_v;&c_a;&c_d;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
    </trdgrp>
  <trdgrp lng="en">
    <trd>cistern</trd>,
    <trd>reservoir</trd>,
    <trd>tank</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#38632;&#27700;&#12384;&#12417;  <pr>&#12358;&#12377;&#12356;&#12384;&#12417;</pr></trd>,
    <trd>&#36015;&#27700;&#27744;  <pr>&#12385;&#12423;&#12377;&#12356;&#12385;</pr></trd>,
    <trd>&#27700;&#27133;  <pr>&#12377;&#12356;&#12381;&#12358;</pr></trd>,
    <trd>&#12479;&#12531;&#12463;  <pr>&#12383;&#12435;&#12367;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_c;&c_i;&c_s;&c_t;&c_je;&c_r;&c_n;&c_a;</trd>,
    <trd>&c_b;&c_o;&c_ch;&c_k;&c_a; (&c_d;&c_l;&c_ja; &c_d;&c_u;&c_sh;&c_u; &c_ib; &c_t;. &c_p;.)</trd>,
    <trd>&c_b;&c_a;&c_k;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_v;&c_o;&c_k;</trd>,
    <trd>&c_b;&c_a;&c_s;&c_je;&c_j;&c_n;</trd>,
    <trd>&c_n;&c_je;&c_v;&c_je;&c_l;&c_i;&c_k;&c_a; &c_v;&c_o;&c_d;&c_o;&c_j;&c_m;&c_a;</trd>
  </trdgrp>
</drv>
</art>
