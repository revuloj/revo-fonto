<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>cistern</rad>/o
</kap>

<drv mrk="cister.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Rezervujo por pluvakvo, ordinare subtera.
    </dif>
    <ref tip="vid" cel="basen.0o">baseno</ref>
    <trd lng="de">Zisterne</trd>
    <trd lng="hu">ciszterna</trd>
    <trdgrp lng="nl">
      <trd>cisterne</trd>,
      <trd>regenput</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>cysterna</trd>,
      <trd>zbiornik</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <dif>
      Rezervujo por nafto.
    </dif>
    <trd lng="hu">k&odblac;olajtart&aacute;ly</trd>
    <trd lng="de">Tank</trd>
    <trd lng="nl">tank</trd>
    <trdgrp lng="pl">
      <trd>cysterna</trd>,
      <trd>zbiornik</trd>,
      <trd>bak</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_s;&c_t;&c_e;&c_r;&c_n;&c_a;</trd>
  <trd lng="es">cisterna</trd>
  <trd lng="fr">citerne</trd>
  <trd lng="it">cisterna</trd>
  <trd lng="pt">cisterna</trd>
  <trdgrp lng="ru">
    <trd>&c_c;&c_i;&c_s;&c_t;&c_je;&c_r;&c_n;&c_a;</trd>,
    <trd>&c_b;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">cisterna</trd>
</drv>
</art>
