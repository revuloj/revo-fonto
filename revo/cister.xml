<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cister.xml,v 1.21 2021/03/06 06:20:01 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>cistern</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="cister.0o">
  <kap><tld/>o</kap>
  <snc mrk="cister.0o.AKV">
    <dif>
      Rezervujo por pluvakvo, ordinare subtera:
      <ekz>
        <klr>[la]</klr> korto entenis iun monstran <tld/>on
        el lado, de kiu plurfoje tage mi devis eltiri akvon
        <fnt>
          <aut>A. LeQuint</aut>,
          <vrk>Infantraviva&jcirc;oj</vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="basen.0o.HOR">baseno <sncref/></ref>,
      <ref cel="kuv.0o.KOMUNE">kuvo</ref>
    </refgrp>
    <trd lng="de">Zisterne</trd>
    <trd lng="hu">ciszterna</trd>
    <trdgrp lng="nl">
      <trd>cisterne</trd>,
      <trd>regenput</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>cysterna</trd>,
      <trd>zbiornik</trd>
    </trdgrp>
  </snc>
  <snc mrk="cister.0o.NAF">
    <dif>
      Rezervujo por nafto.
    </dif>
    <trd lng="hu">k&odblac;olajtart&aacute;ly</trd>
    <trd lng="de">Tank</trd>
    <trd lng="nl">tank</trd>
    <trdgrp lng="pl">
      <trd>cysterna</trd>,
      <trd>zbiornik</trd>,
      <trd>bak</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_s;&c_t;&c_e;&c_r;&c_n;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>cisterna</trd>,
    <trd>dip&ograve;sit</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>cisterna</trd>,
    <trd>k&#xE1;&#x10F;</trd>,
    <trd>n&#xE1;dr&#x17E;</trd>,
    <trd>rezervo&#xE1;r</trd>,
    <trd>z&#xE1;sobn&#xED;k</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cistern</trd>,
    <trd>reservoir</trd>,
    <trd>tank</trd>
  </trdgrp>
  <trd lng="es">cisterna</trd>
  <trd lng="fr">citerne</trd>
  <trd lng="he">&#x5DE;&#x5D9;&#x5DB;&#x5DC;</trd>
  <trd lng="it">cisterna</trd>
  <trd lng="pt">cisterna</trd>
  <trdgrp lng="ru">
    <trd>&c_c;&c_i;&c_s;&c_t;&c_je;&c_r;&c_n;&c_a;</trd>,
    <trd>&c_b;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>cisterna</trd>,
    <trd>n&#xE1;dr&#x17E;</trd>
  </trdgrp>
</drv>
</art>
