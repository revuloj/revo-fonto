<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: saga.xml,v 1.8 2023/06/27 21:26:48 revo Exp $">
<kap>
  <rad>saga</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="saga.0o">
  <kap><tld/>o</kap>
  <snc mrk="saga.0o.BELE">
    <uzo tip="fak">BELE</uzo>
    <dif>
      Proza rakonto &gcirc;enerale temanta pri fama homo,
      kiaj verki&gcirc;is en Islando en la 12a kaj 13a jarcentoj:
      <ekz>
        la <ctl><tld lit="S"/>o de Njal</ctl>;
      </ekz>
      <ekz>
        la <ctl><tld lit="S"/>o de la Volsungoj</ctl>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_g;&c_a; <klr tip="amb">(&c_t;&c_v;&c_o;&c_r;)</klr></trd>
  <trd lng="cs">s&#xE1;ga <klr>(skand. pov&#x11B;st)</klr></trd>
  <trd lng="de">Sage</trd>
  <trd lng="en">saga</trd>
  <trd lng="fr">saga</trd>
  <trd lng="it">saga</trd>
  <trd lng="nl">sage</trd>
  <trd lng="no">saga</trd>
  <trdgrp lng="pl">
    <trd>saga</trd>,
    <trd>podanie</trd>
  </trdgrp>
  <trd lng="sk">s&#xE1;ga <klr>(&scaron;kandin&#xE1;vska poves&tcaron;)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_a;&c_g;&c_a;</trd>,
    <trd>&c_s;&#x43A;&c_a;&c_z;&c_a;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>
</art>

