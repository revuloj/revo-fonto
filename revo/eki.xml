<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: eki.xml,v 1.21 2023/08/31 11:49:27 revo Exp $">
<kap>
  <rad>eki</rad>/o
</kap>
<drv mrk="eki.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="boraga.0oj">boragacoj</ref>
      (<trd lng="la">Echium</trd>), kun bluaj k blankaj floroj:
      <ekz>
        <tld/>o: specioj estas unu a&ubreve; plurjaraj herboj kaj arbedoj, precipe
        disvastigitaj en la mediteraneaj landoj, kun nesimetrie funelformaj floroj,
        aran&gcirc;itaj en ofte grandaj cumoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19917">Boragacoj</url></lok>
        </fnt>;
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/3/31/EchiumVulgare-CloseUp-hr.jpg/320px-EchiumVulgare-CloseUp-hr.jpg" prm="CC-BY-SA-3.0">floro de ordinara <tld/>o (Echium vulgare)
        <fnt>
          <lok><url ref="&WCW;/File:EchiumVulgare-CloseUp-hr.jpg">&WCW;/File:EchiumVulgare-CloseUp-hr.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="be">
      <trd>&c_r;&c_u;&c_m;&c_ja;&c_n;&c_k;&c_a;</trd>,
      <trd>&c_s;&c_ib;&c_n;&c_ja;&c_k;&c_v;&c_je;&c_t;</trd>,
      <trd>&c_v;&c_y;&c_r;&c_o;&c_ch;&c_k;&c_a;</trd>,
      <trd>&c_p;&c_a;&c_d;&c_v;&c_ja;&c_r;&c_e;&c_d;&c_n;&c_ib;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>&egrave;quium</trd>,
      <trd>bolenga</trd>,
      <trd>llengua de bou</trd>,
      <trd>taginaste</trd>
    </trdgrp>
    <trd lng="de">Natterkopf</trd>
    <trdgrp lng="fa">
      <trd>&f_g;&a_l; &f_g;&a_A;&a_w;&a_z;&a_b;&a_A;&a_n;</trd>,
      <trd>&a_A;&a_f;&a_ayn;&f_y;&zwnj;&f_g;&f_y;&a_A;&a_h;</trd>
    </trdgrp>
    <trd lng="fr">vip&eacute;rine</trd>
    <trdgrp lng="hu">
      <trd>k&iacute;gy&oacute;szisz</trd>,
      <trd>terj&odblac;ke</trd>
    </trdgrp>
      <trd lng="pl">&zdot;mijowiec</trd>
      <trd lng="pt">borragem</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_u;&c_m;&c_ja;&c_n;&c_k;&c_a;</trd>,
      <trd>&c_s;&c_i;&c_n;&c_ja;&c_k;</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">hadinec obecn&yacute;</trd>
  <trd lng="he">&#x5E2;&#x5DB;&#x5E0;&#x5D0;&#x5D9;</trd>
  <trd lng="ja">&#x30B7;&#x30E3;&#x30BC;&#x30F3;&#x30E0;&#x30E9;&#x30B5;&#x30AD;</trd>
  <trd lng="sk">hadinec</trd>
  <trd lng="uk">&#x441;&#x438;&#x43D;&#x44F;&#x43A;</trd>
</drv>
</art>
