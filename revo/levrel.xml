<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: levrel.xml,v 1.11 2019/12/13 20:10:22 revo Exp $">
<kap>
  <rad>levrel</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="levrel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="levrel.0o.leporhundo">
    <dif>
      <ref tip="dif" cel="hund.lepor0o">Leporhundo</ref>:
      <ekz>
        &ccirc;iun renkontitan hundon li nomis vertago, e&ccirc; se temis
        pri <tld/>o a&ubreve; mopso
        <fnt><bib>DonKihxoto</bib> <lok>dua parto, anta&ubreve;parolo al la
        leganto, p. 394a</lok></fnt>;
      </ekz>
      <ekz>
        hidalgo <klr>[&#8230;]</klr> kun lanco en rako, antikva &scirc;ildo, osta
        &ccirc;evala&ccirc;o kaj
        rapida <tld/>o
        <fnt>
          <vrk><url
          ref="https://www.meneame.net/m/cultura/castigame-madre-yo-trompogelas"
              >Cast&iacute;game mi madre, y yo tr&oacute;mpogelas</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_n;&c_ch;&c_a;&c_k;</trd>
  <trd lng="ca">llebrer</trd>
  <trdgrp lng="cs">
    <trd>chrt</trd>,
    <trd>loveck&yacute; pes</trd>,
    <trd>oha&rcaron;</trd>
  </trdgrp>
  <trd lng="de">Windhund</trd>
  <trd lng="es">perro lebrel</trd>
  <trd lng="fr">l&eacute;vrier</trd>
  <trd lng="hu">ag&aacute;r</trd>
  <trd lng="nl">windhond</trd>
  <trd lng="pl">chart</trd>
  <trdgrp lng="pt">
    <trd>galgo</trd>,
    <trd>lebr&eacute;u</trd>
  </trdgrp>
  <trd lng="ro">ogar</trd>
  <trd lng="sk">chrt</trd>
</drv>
</art>

