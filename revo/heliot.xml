<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: heliot.xml,v 1.23 2023/10/16 05:12:01 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>heliotrop</rad>/o
</kap>
<drv mrk="heliot.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="heliot.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio <ref tip="malprt" cel="boraga.0oj">boragacoj</ref>
      (<trd lng="la">Heliotropium</trd>):
      <ekz>
        aromas la <tld/>' sen fin'
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=12163">Elektitaj Versa&jcirc;oj de Marie Under</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        specioj de <tld/>o estas herboj a&ubreve; arbedoj kun forte bonodoraj blankaj,
        purpur-violkoloraj floroj, kiuj turni&gcirc;as la&ubreve; la suno
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19917">Boragacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/6/69/Heliotropium_indicum_(2410170156).jpg/320px-Heliotropium_indicum_(2410170156).jpg" prm="CC BY-SA 2.0">Hinda <tld/>o (Heliotropium indicum)
        <fnt>
          <aut>Dinesh Valke el Thane, Barato</aut>
          <lok><url ref="&WCW;/File:Heliotropium_indicum_(2410170156).jpg">&WCW;/File:Heliotropium_indicum_(2410170156).jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Sonnenwende <klr>(Pflanze)</klr></trd>
    <trd lng="el">&eta;&lambda;&jota;&omikron;&tau;&rho;&omikron_ton;&pi;&jota;&omikron;</trd>
    <trd lng="es">heliotropo</trd>
    <trd lng="fr">h&eacute;liotrope</trd>
    <trd lng="hu">kunkor<klr tip="ind"> (n&ouml;v&eacute;ny)</klr></trd>
    <trd lng="it">eliotropo</trd>
    <trd lng="pt">heliotr&oacute;pio</trd>
  </snc>
  <ref tip="hom" cel="heliot1.0o">heliotropo (speco de kvarco)</ref>
  <trd lng="be">&c_g;&c_je;&c_l;&c_ib;&c_ja;&c_t;&c_r;&c_o;&c_p;</trd>
  <trd lng="en">heliotrope<klr> (plant)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x30AD;&#x30C0;&#x30C1;&#x30EB;&#x30EA;&#x30BD;&#x30A6;</trd>,
    <trd>&#x30D8;&#x30EA;&#x30AA;&#x30C8;&#x30ED;&#x30FC;&#x30D7;</trd>,
    <trd>&#x8840;&#x77F3;  <pr>&#x3051;&#x3063;&#x305B;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="nl">heliotroop</trd>
  <trd lng="ru">&c_g;&c_je;&c_l;&c_i;&c_o;&c_t;&c_r;&c_o;&c_p;</trd>
  <trdgrp lng="uk">
    <trd>&#x433;&#x435;&#x43B;&#x456;&#x43E;&#x442;&#x440;&#x43E;&#x43F;</trd>,
    <trd>&#x433;&#x435;&#x43B;&#x456;&#x43E;&#x442;&#x440;&#x43E;&#x43F;</trd>
  </trdgrp>
</drv>
</art>
