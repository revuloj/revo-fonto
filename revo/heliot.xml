<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>heliotrop</rad>/o
</kap>
<drv mrk="heliot.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="boraga.0oj">boragacoj</ref>
      (<trd lng="la">Heliotropium</trd>).
    </dif>
    <trd lng="de">Sonnenwende</trd>
  </snc>
  <snc>
    <uzo tip="fak">MIN</uzo>
    <dif>
      Speco de amorfa kvarco.
    </dif>
    <trd lng="de">Blutjaspis</trd>
  </snc>
</drv>
</art>

