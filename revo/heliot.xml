      Speco de amorfa kvarco.
    </dif>
    <trd lng="de">Blutjaspis</trd>
    <trd lng="hu">heliotrop</trd>
  </snc>
  <trd lng="nl">heliotroop</trd>
  <trd lng="ru">&c_g;&c_je;&c_l;&c_i;&c_o;&c_t;&c_r;&c_o;&c_p;</trd>
</drv>
