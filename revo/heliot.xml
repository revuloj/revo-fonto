<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>heliotrop</rad>/o
</kap>
<drv mrk="heliot.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="boraga.0oj">boragacoj</ref>
      (<trd lng="la">Heliotropium</trd>).
    </dif>
    <trd lng="de">Sonnenwende</trd>
    <trd lng="el">&eta;&lambda;&jota;&omikron;&tau;&rho;&omikron_ton;&pi;&jota;&omikron;</trd>
    <trd lng="es">heliotropo</trd>
    <trd lng="fr">h&eacute;liotrope</trd>
    <trd lng="hu">kunkor<klr tip="ind"> (n&ouml;v&eacute;ny)</klr></trd>
    <trd lng="pt">heliotr&oacute;pio</trd>
  </snc>
  <snc>
    <uzo tip="fak">MIN</uzo>
    <dif>
      Speco de amorfa kvarco.
    </dif>
    <trd lng="de">Blutjaspis</trd>
    <trd lng="hu">heliotrop</trd>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_l;&c_ib;&c_ja;&c_t;&c_r;&c_o;&c_p;</trd>
  <trd lng="en">heliotrope</trd>
  <trd lng="nl">heliotroop</trd>
  <trd lng="ru">&c_g;&c_je;&c_l;&c_i;&c_o;&c_t;&c_r;&c_o;&c_p;</trd>
</drv>
</art>
