<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: heliot.xml,v 1.18 2021/05/23 14:51:35 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>heliotrop</rad>/o
</kap>
<drv mrk="heliot.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="heliot.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio <ref tip="malprt" cel="boraga.0oj">boragacoj</ref>
      (<trd lng="la">Heliotropium</trd>):
      <ekz>
        aromas la <tld/>' sen fin'
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=12163"
              >Elektitaj Versa&jcirc;oj de Marie Under</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        specioj de <tld/>o estas herboj a&ubreve; arbedoj kun forte bonodoraj blankaj,
        purpur-violkoloraj floroj, kiuj turni&gcirc;as la&ubreve; la suno
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19917"
              >Boragacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/6/69/Heliotropium_indicum_(2410170156).jpg/320px-Heliotropium_indicum_(2410170156).jpg"
      prm="CC BY-SA 2.0"
      >Hinda <tld/>o (Heliotropium indicum)
        <fnt>
          <aut>Dinesh Valke el Thane, Barato</aut>
          <lok><url ref="&WCW;/File:Heliotropium_indicum_(2410170156).jpg"
              >https://commons.wikimedia.org/wiki/File:Heliotropium_indicum_(2410170156).jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Sonnenwende <klr>(Pflanze)</klr></trd>
    <trd lng="el">&eta;&lambda;&jota;&omikron;&tau;&rho;&omikron_ton;&pi;&jota;&omikron;</trd>
    <trd lng="es">heliotropo</trd>
    <trd lng="fr">h&eacute;liotrope</trd>
    <trd lng="hu">kunkor<klr tip="ind"> (n&ouml;v&eacute;ny)</klr></trd>
    <trd lng="it">eliotropo</trd>
    <trd lng="pt">heliotr&oacute;pio</trd>
  </snc>
  <ref tip="hom" cel="heliot1.0o">heliotropo (speco de kvarco)</ref>
  <trd lng="be">&c_g;&c_je;&c_l;&c_ib;&c_ja;&c_t;&c_r;&c_o;&c_p;</trd>
  <trd lng="en">heliotrope</trd>
  <trd lng="nl">heliotroop</trd>
  <trd lng="ru">&c_g;&c_je;&c_l;&c_i;&c_o;&c_t;&c_r;&c_o;&c_p;</trd>
</drv>
</art>
