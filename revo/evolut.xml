<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: evolut.xml,v 1.12 2023/11/07 20:34:12 revo Exp $">
<kap>
  <rad>evolut</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="evolut.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="volv.el0ato">Elvolvato</ref>:
      <ekz>
        <tld/>o de kurbo estas la situo de &ccirc;iuj &gcirc;iaj kurbecocirkloj
        (centroj de kurbeco), ekvivalente, &gcirc;i estas la koverto de
        perpendikularoj al la fonta kurbo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=260027">Evoluto</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_v;&c_a;&c_l;&c_ju;&c_t;&c_a;</trd>
  <trd lng="bg">&c_je;&c_v;&c_o;&c_l;&c_ju;&c_t;&c_a;</trd>
  <trd lng="cs">evoluta</trd>
  <trd lng="de">Evolute</trd>
  <trd lng="en">evolute</trd>
  <trd lng="fr">d&eacute;velopp&eacute;e<klr tip="ind"> (d'une courbe)</klr></trd>
  <trd lng="hu">evol&uacute;ta</trd>
  <trd lng="nl">evolute</trd>
  <trd lng="pl">ewoluta</trd>
  <trd lng="pt">evoluta</trd>
  <trd lng="ru">&c_e;&c_v;&c_o;&c_l;&c_ju;&c_t;&c_a;</trd>
  <trd lng="sk">evoluta</trd>
  <trd lng="uk">&#x435;&#x432;&#x43E;&#x43B;&#x44E;&#x442;&#x430;</trd>
  <trd lng="zh">&#x6E10;&#x5C48;&#x7EBF; <pr>ji&#xE0;nq&#x16B;xi&#xE0;n</pr></trd>
</drv>
</art>

