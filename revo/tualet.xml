<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tualet</rad>/o
</kap>
<drv mrk="tualet.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Tuto de la objektoj, per kiuj oni sin vestas:
      <ekz>
        simpla, eleganta, ceremonia <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="galant2.0o">galanterio</ref>.
    </refgrp>
    <trd lng="de">Kleidung</trd>
    <trd lng="nl">kleding</trd>
  </snc>
  <snc>
    <dif>
      Tuto de la a&jcirc;oj kaj aran&gcirc;oj necesaj por sinvestado kaj sinpurigado:
      <ekz>
        <ind>fari sian <tld/>on</ind>.
        <trdgrp lng="hu">
          <trd>fel&ouml;lt&ouml;zk&ouml;dik</trd>,
          <trd>kicsinos&iacute;tja mag&aacute;t</trd>
        </trdgrp>
      </ekz>
    </dif>
    <trd lng="nl">toilet</trd>
  </snc>
  <trd lng="fr">toilette</trd>
  <trdgrp lng="hu">
    <trd>toalett</trd>,
    <trd>&ouml;lt&ouml;z&eacute;k</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_u;&c_a;&c_l;&c_je;&c_t; <klr tip="amb">(&c_n;&c_a;&c_r;&c_ja;&c_d;, &c_o;&c_d;&c_je;&c_zh;&c_d;&c_a;)</klr></trd>
</drv>

<drv mrk="tualet.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      &Ccirc;ambro por sinpurigado kaj sinvestado.
    </dif>
  </snc>
  <trd lng="fr">toilettes</trd>
  <trd lng="hu">&ouml;lt&odblac;z&odblac;szoba</trd>  
  <trd lng="nl">kleedkamer</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_u;&c_a;&c_l;&c_je;&c_t;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_m;&c_n;&c_a;&c_t;&c_a; <klr tip="amb">(&c_d;&c_l;&c_ja; &c_o;&c_d;&c_je;&c_v;&c_a;&c_n;&c_i;&c_ja;)</klr></trd>,
    <trd>&c_u;&c_b;&c_o;&c_r;&c_n;&c_a;&c_ja; <klr tip="amb">(&c_a;&c_r;&c_t;&c_i;&c_s;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja;)</klr></trd>
  </trdgrp>
</drv>
</art>
