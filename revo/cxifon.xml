<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>&ccirc;ifon</rad>/o
</kap>
<drv mrk="cxifon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Difektita, eluzita peco da teksa&jcirc;o, da vesto, kc. 
    </dif>
    <refgrp tip="sin">
      <ref cel="cxif.0ajxo">&ccirc;ifa&jcirc;o</ref>.
    </refgrp>
  </snc>
  <snc num="2">
    <dif>
      Senvalora  &scirc;irpeco, senvalora&jcirc;o:
      <ekz>
        <tld/>a vesto
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
      </ekz>
      <ekz>
        sanga <tld/>o de mortinta besto<fnt>K</fnt>;
      </ekz>
      <ekz>
        la politikaj traktatoj ofte fari&gcirc;as nuraj <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Lumpen</trd>,
    <trd>Lappen</trd>,
    <trd>Fetzen</trd>
   </trdgrp>
  <trd lng="en">rag</trd>
</drv>
<drv mrk="cxifon.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Metia kolektanto de <tld/>oj. 
    </dif>
  </snc>
  <trd lng="de">Lumpensammler</trd>
</drv>
<drv mrk="cxifon.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Homo vestita per <tld/>oj. 
    </dif>
  </snc>
  <trdgrp lng="de"><trd>Lump</trd>, <trd>Bettler</trd></trdgrp>
</drv>
<drv mrk="cxifon.0figuro">
  <kap><tld/>figuro</kap>
  <snc>
    <dif>
      <tld/>a figuro proksimume similanta homon, starigita en &gcirc;ardeno
      por fortimigi la birdojn.
    </dif>
  </snc>
  <trd lng="de">Vogelscheuche</trd>
</drv>
</art>
<!--
$Log$
-->
</vortaro>