<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: konjek.xml,v 1.23 2022/07/16 16:40:48 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>konjekt</rad>/i
</kap>
<drv mrk="konjek.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Elmeti opinion, konkludon a&ubreve; klarigon bazitan nur sur 
      probabla&jcirc;oj:
      <ekz>
        la rezultatojn oni e&ccirc; ne povas <tld/>i;
      </ekz>
      <ekz>
        la homa spirito emas <tld/>i pri tio, kion li ne scias.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="supoz.0i">supozi</ref>,
      <ref cel="diven.0i">diveni</ref>,
      <ref cel="suspek.0i">suspekti</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_d;&c_a;&c_g;&c_a;&c_d;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_m;&c_je;&c_r;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd
  lng="bo">&#xF5A;&#xF7C;&#xF51;&#xF0B;&#xF51;&#xF54;&#xF42;&#xF0B;&#xF56;&#xFB1;&#xF7A;&#xF51;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>domn&iacute;vat se</trd>,
    <trd>dom&yacute;&scaron;let se</trd>,
    <trd>p&rcaron;edpokl&aacute;dat</trd>,
    <trd>tu&scaron;it</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>mutma&szlig;en</trd>,
    <trd>vermuten</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>make a <ind>conjecture</ind></trd>,
    <trd>guess</trd>,
    <trd>suppose</trd>,
    <trd>surmise</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>hacer una <ind>conjetura</ind></trd>,
    <trd>conjeturar</trd>,
    <trd>presumir</trd>,
    <trd>suponer</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_H;&a_d;&a_s; &a_z;&a_d;&a_n;</trd>,
    <trd>&a_t;&a_S;&a_w;&a_r; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&f_g;&a_m;&a_A;&a_n; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_A;&a_H;&a_t;&a_m;&a_A;&a_l; &a_d;&a_A;&a_d;&a_n;</trd>,
    <trd>&f_p;&a_n;&a_d;&a_A;&a_sh;&a_t;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>conjecturer</trd>,
    <trd>pr&eacute;sumer</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>veronderstellen</trd>,
    <trd>gissen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>domiarkowa&cacute; si&eogonek;</trd>,
    <trd>domniemywa&cacute;</trd>,
    <trd>domy&sacute;la&cacute; si&eogonek;</trd>,
    <trd>przypuszcza&cacute;</trd>,
    <trd>robi&cacute; za&lstroke;o&zdot;enie</trd>,
    <trd>wyczu&cacute;</trd>,
    <trd>zak&lstroke;ada&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>conjeturar</trd>,
    <trd>supor</trd>,    
    <trd>calcular</trd>,    
    <trd>presumir</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>presupune</trd>,
    <trd>ghici</trd>,
    <trd>s&abreve; presupunem</trd>,
    <trd>s&abreve; facem ipoteze</trd>,
    <trd>s&abreve; sim&#x21B;im</trd>,
    <trd>s&abreve; ne &icirc;mp&abreve;c&abreve;m</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_g;&c_a;&c_d;&c_y;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_p;&c_o;&c_l;&c_a;&c_g;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>domnieva&tcaron; sa</trd>,
    <trd>predpoklada&tcaron;</trd>,
    <trd>tu&scaron;i&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="konjek.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Supoza sed ne certa opinio, konkludo a&ubreve; klarigo:
      <ekz>
        oni vane faris tiujn eblajn <tld/>ojn pri lia morto.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_d;&c_a;&c_g;&c_a;&c_d;&c_k;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Mutma&szlig;ung</trd>,
      <trd>Vermutung</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>conjecture</trd>,
      <trd>guess</trd>,
      <trd>supposition</trd>,
      <trd>surmise</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_H;&a_d;&a_s;</trd>,
      <trd>&f_g;&a_m;&a_A;&a_n;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>conjecture</trd>,
      <trd>pr&eacute;somption</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>gissing</trd>,
      <trd>veronderstelling</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>domys&lstroke;</trd>,
      <trd>hipoteza</trd>,
      <trd>przypuszczenie</trd>,
      <trd>za&lstroke;o&zdot;enie</trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>ghici</trd>,
      <trd>ipotez&abreve;</trd>,
      <trd>presupunere</trd>,
      <trd>supozi&#x21B;ie</trd>,
      <trd>funda&#x21B;ie</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_o;&c_g;&c_a;&c_d;&c_k;&c_a;</trd>,
      <trd>&c_p;&c_r;&c_je;&c_d;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc mrk="konjek.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <dif>
      Aserto, kiun oni opinias vera, sed ankora&ubreve; ne sukcesis
      <ref cel="demons.0i">demonstri</ref>:
      <ekz>
        la <ind><tld/>o pri la kvar koloroj</ind> ricevis solvon
        helpe de komputilo;
        <trd lng="de">Vierfarbenvermutung</trd>
        <trd lng="en">four-colour <ind>conjecture</ind></trd>
        <trd lng="fa">&a_H;&a_d;&a_s; &f_ch;&a_h;&a_A;&a_r;&a_r;&a_n;&f_g;</trd>
        <trdgrp lng="fr">
          <trd>conjecture des quatre couleurs</trd>,
          <trd>probl&egrave;me des quatre couleurs</trd></trdgrp> 
        <trdgrp lng="nl">
          <trd>vierkleurenvermoeden</trd>,
          <trd>vierkleurenhypothese</trd>
        </trdgrp>
        <trd lng="pl">zagadnienie czterech barw</trd>
        <trd lng="ro">problema a patru culori</trd>
        <trd lng="ru">&c_g;&c_i;&c_p;&c_o;&c_t;&c_je;&c_z;&c_a; &c_ch;&c_je;&c_t;&c_y;&c_r;&c_jo;&c_h; &c_k;&c_r;&c_a;&c_s;&c_o;&c_k;</trd>
      </ekz>
      <ekz>
        &gcirc;is anta&ubreve; nelonge la t.n. <ctl>teoremo de Fermat</ctl>
        estis nura <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aksiom.0o.MAT">aksiomo</ref>,
      <ref cel="teorem.0o">teoremo</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_z;&c_d;&c_a;&c_g;&c_a;&c_d;&c_k;&c_a;</trd>,
      <trd>&c_g;&c_ib;&c_p;&c_o;&c_t;&c_e;&c_z;&c_a;</trd>
    </trdgrp>
    <trd lng="de">Vermutung</trd>
    <trd lng="en">conjecture</trd>
    <trdgrp lng="fa">
      <trd>&a_H;&a_d;&a_s;</trd>,
      <trd>&a_H;&a_d;&a_s;&f_y;&a_h;</trd>
    </trdgrp>
    <trd lng="fr">conjecture</trd>
    <trdgrp lng="nl">
      <trd>vooronderstelling</trd>,
      <trd>hypothese</trd>
    </trdgrp>
    <trd lng="pl">hipoteza</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_je;&c_d;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_g;&c_i;&c_p;&c_o;&c_t;&c_je;&c_z;&c_a;</trd>
    </trdgrp>
  </snc>
  <trd lng="bo">&#xF5A;&#xF7C;&#xF51;&#xF0B;&#xF51;&#xF54;&#xF42;&#xF0B;&#xF54;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>dohad</trd>,
    <trd>domn&ecaron;nka</trd>,
    <trd>konjektura</trd>,
    <trd>p&rcaron;edpoklad</trd>
  </trdgrp>
  <trdgrp lng="es">
      <trd>conjetura</trd>,
      <trd>suposici&oacute;n</trd>
    </trdgrp>
  <trdgrp lng="sk">
    <trd>dohad</trd>,
    <trd>domnienka</trd>
  </trdgrp>
</drv>

<drv mrk="konjek.0ebla">
  <kap><tld/>ebla</kap>
  <snc>
    <dif>
      Probabla:
      <ekz>
        lia sukceso estas <tld/>ebla.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_g;&c_ch;&c_y;&c_m;&c_y;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_a;&c_g;&c_o;&c_d;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>konjektur&aacute;ln&iacute;</trd>,
    <trd>pravd&ecaron;podobn&yacute;</trd>,
    <trd>p&rcaron;edpokl&aacute;dan&yacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>presumable</trd>,
    <trd>supposable</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_q;&a_A;&a_b;&a_l; &a_f;&a_r;&a_D;</trd>,
    <trd>&a_m;&a_H;&a_t;&a_m;&a_l;</trd>
  </trdgrp>
  <trd lng="fr">pr&eacute;sumable</trd>
  <trd lng="nl">veronderstelbaar</trd>
  <trd lng="pl">przypuszczalny</trd>     
  <trd lng="ro">probabil</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_z;&c_m;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_o;&c_ja;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_p;&c_o;&c_l;&c_a;&c_g;&c_a;&c_je;&c_m;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pravdepodobn&yacute;</trd>,
    <trd>predpokladan&yacute;</trd>
  </trdgrp>
</drv>
</art>
