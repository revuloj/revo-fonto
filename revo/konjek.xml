    <trd>&#25512;&#23450;&#12377;&#12427;  <pr>&#12377;&#12356;&#12390;&#12356;&#12377;&#12427;</pr></trd>,
    <trd>&#25014;&#28204;&#12377;&#12427;  <pr>&#12362;&#12367;&#12381;&#12367;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
