<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: regist1.xml,v 1.5 2023/12/07 06:32:10 afido Exp $">
    <kap>
      <rad>registr</rad>/o
      <fnt><bib>PV</bib></fnt>
    </kap>

<drv mrk="regist1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="regist1.0o.orgena">
    <dif>
      Orgena fajfilaro de unu speco, de unu sonkoloro, ampleksanta plurajn oktavojn kaj &scirc;altebla per
      unu movo:
      <ekz>
        <tld/>o 16'-a malfermita, <tld/>o 8'-a fermita.
      </ekz>
    </dif>
    <rim>
      La vorto devenas el mezlatina &#x201A;<nac>registrum</nac>&#8216;, t.e. tir&scirc;nuro de sonorilaro, de
      orgeno.
    </rim>
    <trd lng="be">&c_r;&c_e;&c_g;&c_ib;&c_s;&c_t;&c_a;&c_r;</trd>
  <trd lng="de">Register <klr>(Orgel)</klr></trd>
  </snc>
  <snc mrk="regist1.0o.vocxo">
    <dif>
      Absoluta amplekso de vo&ccirc;o a&ubreve; muzikilo
      de plej malalta al plej alta tonoj, kiujn &gcirc;i
      kapablas sonigi:
      <ekz>
        la mezaj <tld/>oj estas baritono &ccirc;e viroj kaj mezosoprano &ccirc;e virinoj
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kantisto, kies vo&ccirc;a <tld/>o estis kvin oktavoj
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="vocx.0o.registro">vo&ccirc;o<sncref/></ref>
    <trd lng="be">&c_r;&c_e;&c_g;&c_ib;&c_s;&c_t;&c_a;&c_r;</trd>
    <trdgrp lng="de">
      <trd>Register <klr>(Stimme)</klr></trd>,
      <trd>Stimmumfang</trd>
    </trdgrp>
    <trd lng="es">registro</trd>
    <trd lng="nl">register</trd>
    <trd lng="ru">&c_r;&c_je;&c_g;&c_i;&c_s;&c_t;&c_r;</trd>
  </snc>
  <ref tip="hom" cel="regist.0o.dokumento">registro <klr>(dosiero)</klr></ref>
  <trdgrp lng="ja">
    <trd>&#x767B;&#37682;&#x7C3F;  <pr>&#12392;&#12358;&#x308D;&#x304F;&#x307C;</pr></trd>,
    <trd>&#x767B;&#x8A18;&#x7C3F;  <pr>&#12392;&#12358;&#x304D;&#x307C;</pr></trd>,
    <trd>&#x53F0;&#x5E33;  <pr>&#12384;&#12356;&#12385;&#12423;&#12358;</pr></trd>,
    <trd>&#x8A18;&#37682;&#34920;  <pr>&#x304D;&#x308D;&#x304F;&#x304A;&#12418;&#12390;</pr></trd>,
    <trd>&#x58F0;&#x57DF;  <pr>&#x305B;&#12356;&#12356;&#x304D;</pr></trd>,
    <trd>&#x97F3;&#x57DF;  <pr>&#x304A;&#12435;&#12356;&#x304D;</pr></trd>
  </trdgrp>
</drv>

  </art>

