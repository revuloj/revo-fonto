<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: inkunabl.xml,v 1.6 2022/03/21 15:27:49 revo Exp $">
<kap>
  <rad>inkunabl</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="inkunabl.0o">
  <kap><tld/>o</kap>
  <snc mrk="inkunabl.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Tre frua kaj valora eldona&jcirc;o, ekzemple
      en E&ubreve;ropo <ref tip="super" cel="libr.0o">libro</ref>
      eldonita anta&ubreve; la jaro 1501a:
      <ekz>
        ekzistas homoj kolektantaj po&scirc;tmarkojn, kaj aliaj kolektantaj 
        <tld/>ojn
        <fnt>
        <bib>Salamandroj</bib>,
        <lok>libro 2a, &ccirc;apitro 1a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ctl>La <tld lit="I"/>oj de Esperanto</ctl>
        <klr>(eldono de fruaj esperanta&jcirc;oj fare de Ludovikito)</klr>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>inkun&aacute;bule</trd>,
    <trd>prvotisk</trd>
  </trdgrp>
  <trd lng="de">Inkunabel</trd>
  <trd lng="fr">incunable</trd>
  <trd lng="pt">incun&#xE1;bulo</trd>
  <trd lng="sk">prvotla&ccaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x456;&#x43D;&#x43A;&#x443;&#x43D;&#x430;&#x431;&#x443;&#x43B;&#x430;</trd>,
    <trd>&#x43F;&#x435;&#x440;&#x448;&#x43E;&#x434;&#x440;&#x443;&#x43A;&#x43E;&#x432;&#x430;&#x43D;&#x430; &#x43A;&#x43D;&#x438;&#x433;&#x430;</trd>
  </trdgrp>
</drv>
</art>

