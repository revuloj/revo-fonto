<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>bul</rad>/o
</kap>
<drv mrk="bul.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Peco da ia ajn materio kun proksimuma formo de globo, sfero:
      <ekz>
        <tld/>o el tero, pasto, viando, ne&gcirc;o, papero, pano;
      </ekz>
      <ekz>
        la nuboj <ind>kun<tld/>i&gcirc;i</ind>s en unu solan grandan 
        minacan mason
        <fnt>(<aut>Zamenhof</aut>: <vrk>Fabeloj de Andersen,
        Sova&gcirc;aj cignoj</vrk>)</fnt>.
        <trdgrp lng="fr">
          <trd>se mettre en <ind>boule</ind></trd>,
          <trd>s'<ind>enrouler</ind></trd></trdgrp> 
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>hrouda</trd>,
    <trd>knedl&iacute;k</trd>
  </trdgrp>
  <trd lng="fr">boule</trd>
  <trdgrp lng="de">
    <trd>Klo&szlig;</trd>,
    <trd>Ball</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_o;&c_k;</trd>,
    <trd>&c_k;&c_o;&c_m;</trd>,
    <trd>&c_k;&c_l;&#1105;&c_c;&c_k;&c_a; <klr>(pasta)</klr></trd>,
    <trd>&c_f;&c_r;&c_i;&c_k;&c_a;&c_d;&c_je;&c_l;&c_mol;&c_k;&c_a; <klr>(vianda)</klr></trd>,
    <trd>&c_s;&c_n;&c_je;&c_zh;&c_o;&c_k; <klr>(ne&gcirc;a)</klr></trd>
  </trdgrp>
</drv>
<drv mrk="bul.faden0o">
  <kap>faden<tld/>o</kap>
  <snc>
    <dif>
      <tld/>forma volva&jcirc;o da fadeno. 
    </dif>
    <refgrp tip="vid">
      <ref cel="mas.0o">maso</ref>,
      <ref cel="pilk.0o">pilko</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Kn&auml;uel</trd>
  <trd lng="fr">pelote <klr tip="ind">(de fil, etc.)</klr></trd>
  <trd lng="ru">&c_k;&c_l;&c_u;&c_b;&c_o;&c_k;</trd>
</drv>
</art>
