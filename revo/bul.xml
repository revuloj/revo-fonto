<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>bul</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="bul.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bul.0o.KOMUNE">
    <dif>
      Peco da ia ajn materio kun proksimuma formo de <ref tip="vid"
      cel="glob.0o"></ref>globo, <ref tip="vid"
      cel="sfer.0o.KOMUNE">sfero</ref>:
      <ekz>
	<tld/>o el tero, pasto, viando, ne&gcirc;o, papero, pano.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_m;&c_ja;&c_k;</trd>,
    <trd>&c_k;&c_l;&c_jo;&c_c;&c_k;&c_a; <klr>(pasta)</klr></trd>,
    <trd>&c_f;&c_r;&c_y;&c_k;&c_a;&c_d;&c_e;&c_l;&c_mol;&c_k;&c_a; <klr>(vianda)</klr></trd>,
    <trd>&c_s;&c_mol;&c_n;&c_ja;&c_zh;&c_o;&c_k; <klr>(ne&gcirc;a)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hrouda</trd>,
    <trd>knedl&iacute;k</trd>
  </trdgrp>
  <trd lng="es">bola</trd>
  <trdgrp lng="en">
    <trd>ball</trd>, <trd>lump</trd>
  </trdgrp>
  <trd lng="fr">boule</trd>
  <trdgrp lng="de">
    <trd>Klo&szlig;</trd>,
    <trd>Ball</trd>
  </trdgrp>
  <trd lng="hu">gomb&oacute;c</trd>
  <trd lng="it">boccia</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_o;&c_k;</trd>,
    <trd>&c_k;&c_o;&c_m;</trd>,
    <trd>&c_k;&c_l;&c_jo;&c_c;&c_k;&c_a; <klr>(pasta)</klr></trd>,
    <trd>&c_f;&c_r;&c_i;&c_k;&c_a;&c_d;&c_je;&c_l;&c_mol;&c_k;&c_a; <klr>(vianda)</klr></trd>,
    <trd>&c_s;&c_n;&c_je;&c_zh;&c_o;&c_k; <klr>(ne&gcirc;a)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="bul.0igxi">
  <kap><tld/>i&gcirc;i, <var><kap>kun<tld/>i&gcirc;i</kap></var></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="bul.0igxi.KOMUNE">
    <dif>
      Formi&gcirc;i kiel <tld/>o, kunpremi&gcirc;ante:
      <ekz>
	la nuboj kun<tld/>i&gcirc;is en unu solan grandan
	minacan mason
	<fnt><bib>Fab1</bib>, <lok>Sova&gcirc;aj cignoj</lok></fnt>.
      </ekz>
      <ekz>
	oni trovis &scirc;in, en la soleca lito, <tld/>i&gcirc;inta kiel salikoko
	<fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent
	Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>se <ind>blottir</ind></trd>,
    <trd>se mettre en <ind>boule</ind></trd>,
    <trd>s'<ind>enrouler</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gomolyog</trd>,
    <trd>&ouml;sszeg&ouml;mb&ouml;ly&ouml;dik</trd>
  </trdgrp>
</drv>

<drv mrk="bul.faden0o">
  <kap>faden<tld/>o</kap>
  <snc mrk="bul.faden0o.TEKS">
    <dif>
      <tld lit="B"/>forma volva&jcirc;o da fadeno.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_u;&c_b;&c_o;&c_k;</trd>
  <trd lng="de">Kn&auml;uel</trd>
  <trdgrp lng="en">
    <trd>threadball</trd>,
    <trd>yarnball</trd>
  </trdgrp>
  <trd lng="fr">pelote <klr tip="ind">(de fil, etc.)</klr></trd>
  <trd lng="it">gomitolo</trd>
  <trd lng="hu">gombolyag</trd>
  <trd lng="ru">&c_k;&c_l;&c_u;&c_b;&c_o;&c_k;</trd>
  <trd lng="sv">nystan</trd>
</drv>

<drv mrk="bul.har0o">
  <kap>har<tld/>o</kap>
  <snc mrk="bul.har0o.KOS">
    <dif>
      Kunturna&jcirc;o da haroj <tld/>e noditaj sur la verto a&ubreve; &ccirc;e la nuko:
      <ekz>
	ho, &scirc;ia ebona hararo densa kaj brile glata! Jen hispana pinglo
	en helena har<tld/>o
	<fnt>
	<aut>Guo Moruo, trad. -</aut>,
	<vrk><url ref="http://esperanto.cri.cn/1/2003/12/26/1@2087.htm">
	Karamela Knabino</url></vrk>,
	<lok>&Ccirc;ina Radio Internacia, 2003-12-26</lok>
	</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">chignon</trd>
  <trd lng="it">crocchia</trd>
</drv>
</art>
