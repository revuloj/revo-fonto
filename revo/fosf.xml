<art mrk="$Id$">
<kap>
  <rad>fosf</rad>/o
</kap>
<drv mrk="fosf.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Plimallongigita nomo de <ref tip="sin" cel="fosfor.0o">fosforo</ref>,
      uzata nur en la nomoj de &hcirc;emiaj kombinoj:
      <ekz><ind><tld/>ato</ind><trd lng="de">Phosphat</trd><trd lng="ru">&c_f;&c_o;&c_s;&c_f;&c_a;&c_t;</trd>,</ekz>
      <ekz><ind><tld/>ito</ind><trd lng="de">Phosphit</trd><trd lng="ru">&c_f;&c_o;&c_s;&c_f;&c_i;&c_t;</trd>.</ekz>
   </dif>
  </snc>
  <trd lng="de">Phos-</trd>
  <trd lng="ru">&c_f;&c_o;&c_s;&c_f;&c_o;&c_r;</trd>
</drv>
</art>
