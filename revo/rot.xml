<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rot</rad>/o
</kap>
<drv mrk="rot.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Ta&ccirc;mento da soldatoj.
    </dif>
    <trd lng="de">Rotte</trd>
    <trd lng="nl">rot<klr tip="ind"> (soldaten)</klr></trd>
    <trd lng="ru">&c_r;&c_o;&c_t;&c_a;</trd>
  </snc>
  <snc num="2" mrk="rot.0o.2">
    <dif>
      Aro da kune irantaj homoj a&ubreve; bestoj: 
      <ekz><tld/>o da perdrikoj.</ekz>
    </dif>
    <trdgrp lng="nl">
      <trd>troep</trd>,
      <trd>groep</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_m;&c_p;&c_a;&c_n;&c_i;&c_ja;</trd>,
      <trd>&c_g;&c_r;&c_u;&c_p;&c_p;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc num="3">
    <dif>
      Subdivido de bataliono.  
    </dif>
    <trd lng="de">Kompanie</trd>
    <trd lng="ru">&c_r;&c_o;&c_t;&c_a;</trd>
  </snc>
</drv>
</art>
