<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>oran&gcirc;</rad>/o
</kap>
<uzo tip="fak">BOT</uzo>
<drv mrk="orangx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Frukto de <tld/>ujo, globforma, dol&ccirc;a, parfuma, sukplena, kun dika
      ru&gcirc;flava &scirc;elo:
      <ekz>
        <tld/>kolora.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Apfelsine</trd>,
    <trd>Orange</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>sinaasappel</trd>,
    <trd>appelsien</trd>
  </trdgrp>
  <trd lng="fr">orange</trd>
  <trd lng="pl">pomara&nacute;cza</trd>
  <trd lng="en">orange</trd>
  <trd lng="tr">portakal</trd>
</drv>
<drv mrk="orangx.0arbo0ujo">
  <kap><tld/>arbo, <tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="citrus.0o">citruso</ref>
      (<trd lng="la">Citrus aurantium</trd>).
    </dif>
  </snc>
  <trd lng="de">Apfelsinenbaum</trd>
  <trd lng="fr">oranger</trd>
</drv>
</art>
