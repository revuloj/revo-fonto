<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: orangx.xml,v 1.34 2024/01/30 16:09:26 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>oran&gcirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="orangx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="orangx.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Frukto de <tld/>ujo, globforma, dol&ccirc;a, parfuma, sukplena, kun dika
      ru&gcirc;flava &scirc;elo:
      <ekz>
        mi havas pomon kaj <tld/>on por vi
        <fnt><bib>Gerda</bib>, <lok>&ccirc;apitro 18a</lok></fnt>.
      </ekz>
      <ekz>
        <ind><tld/>kolora</ind>.
        <trd lng="br">ora&ntilde;jez<klr tip="ind"> (liv)</klr></trd>
        <trdgrp lng="ca">
          <trd>taronja</trd>,
          <trd>carabassa<klr> (adj.)</klr></trd>
        </trdgrp>
        <trd lng="sv">orange</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_e;&c_l;&c_mol;&c_s;&c_ib;&c_n;</trd>
  <trd lng="bg">&c_p;&c_o;&c_r;&c_t;&c_o;&c_k;&c_a;&c_l;</trd>
  <trd lng="bo">&#xF5A;&#xF0B;&#xF63;&#xF74;&#xF0B;&#xF58;&#xF0B;</trd>
  <trd lng="br">ora&ntilde;jezenn<klr tip="ind"> (frouezhenn)</klr></trd>
  <trd lng="ca">taronja</trd>
  <trdgrp lng="de">
    <trd>Apfelsine</trd>,
    <trd>Orange</trd>
  </trdgrp>
  <trd lng="el">&pi;&omikron;&rho;&tau;&omikron;&kappa;&alfa_ton;&lambda;&jota;</trd>
  <trd lng="en">orange</trd>
  <trd lng="es">naranja</trd>
  <trd lng="fr">orange</trd>
  <trd lng="fy">sinesappel</trd>
  <trd lng="hu">narancs</trd>
  <trd lng="id">jeruk</trd>
  <trd lng="io">oranjo</trd>
  <trd lng="ja">&#x30AA;&#x30EC;&#x30F3;&#x30B8; <pr>&#x304A;&#x308C;&#x3093;&#x3058;</pr></trd>
  <trdgrp lng="nl">
    <trd>sinaasappel</trd>,
    <trd>appelsien</trd>
  </trdgrp>
  <trd lng="pl">pomara&nacute;cza</trd>
  <trd lng="pt">laranja</trd>
  <trdgrp lng="ro">
    <trd>portocal</trd>,
    <trd>portocal&abreve;</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_p;&c_je;&c_l;&c_mol;&c_s;&c_i;&c_n;</trd>
  <trd lng="sv">apelsin</trd>
  <trd lng="tr">portakal</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43C;&c_je;&c_r;&c_a;&#x43D;&c_je;&c_c;&#x44C;</trd>,
    <trd>&c_b;&c_ib;&c_g;&c_a;&c_r;&c_a;&c_d;&c_ib;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6A59; <pr>ch&#xE9;ng</pr></trd>,
    <trd>&#x6A59;&#x5B50; <pr>ch&#xE9;ngzi</pr></trd>,
    <trd>&#x67F3;&#x4E01; <pr>li&#x1D4;d&#x12B;ng</pr></trd>,
    <trd>&#x67F3;&#x6A59; <pr>li&#x1D4;ch&#xE9;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="orangx.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o.
    </dif>
    <trd lng="be">&c_a;&c_p;&c_e;&c_l;&c_mol;&c_s;&c_ib;&c_n;&c_a;&c_v;&c_y;</trd>
    <trd lng="fy">sinesappel-</trd>
    <trd lng="ja">&#x30AA;&#x30EC;&#x30F3;&#x30B8;&#x306E;
    <pr>&#x304A;&#x308C;&#x3093;&#x3058;&#x306E;</pr></trd>
    <trdgrp lng="nl">
      <trd>sinaasappel-</trd>,
      <trd>appelsien-</trd>
    </trdgrp>
  </snc>
  <snc>
    <ref tip="dif" cel="orangx.0kolora"><tld/>kolora</ref>
    <trd lng="fy">oranje</trd>
    <trd lng="io">oranjea</trd>
    <trd lng="ja">&#x30AA;&#x30EC;&#x30F3;&#x30B8;&#x8272;&#x306E;
    <pr>&#x304A;&#x308C;&#x3093;&#x3058;&#x3044;&#x308D;&#x306E;</pr></trd>
    <trd lng="nl">oranje<klr tip="ind"> (bn.)</klr></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_r;&c_a;&c_n;&c_zh;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_a;&c_r;&c_a;&c_n;&c_ch;&c_a;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="en">orange</trd>
  <trd lng="pl">pomara&nacute;czowy</trd>
  <trd lng="ro">oranj</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43C;&c_a;&c_r;&c_a;&#x43D;&c_ch;&c_je;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_a;&#x43F;&c_je;&#x43B;&#x44C;&c_s;&c_i;&#x43D;&#x43E;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7DF9; <pr>t&#xED;</pr></trd>,
    <trd>&#x7F07; <pr>t&#xED;</pr></trd>,
    <trd>&#x6A58;&#x7D05; <pr>j&#xFA;h&#xF3;ng</pr></trd>,
    <trd>&#x6A58;&#x7EA2; <pr>j&#xFA;h&#xF3;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="orangx.0arbo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="orangx.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Specio de <ref tip="super" cel="citrus.0o">citruso</ref>
      (<trd lng="la">Citrus aurantium</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_e;&c_l;&c_mol;&c_s;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_je; &c_d;&c_r;&c_e;&c_v;&c_a;</trd>
  <trdgrp lng="br">
    <trd>gwezenn-ora&ntilde;jez</trd>,
    <trd>ora&ntilde;jezenn<klr tip="ind"> (gwezenn)</klr></trd>
  </trdgrp>
  <trd lng="ca">taronger</trd>
  <trdgrp lng="de">
    <trd>Apfelsinenbaum</trd>,
    <trd>Orangenbaum</trd>
  </trdgrp>
  <trd lng="el">&pi;&omikron;&rho;&tau;&omikron;&kappa;&alfa;&lambda;&jota;&alfa_ton;</trd>
  <trd lng="en">orange tree</trd>
  <trd lng="fr">oranger</trd>
  <trdgrp lng="fy">
    <trd>sinesappelbeam</trd>,
    <trd>oranjebeam</trd>
  </trdgrp>
  <trd lng="hu">narancsfa</trd>
  <trd lng="ja">&#x30AA;&#x30EC;&#x30F3;&#x30B8;&#x306E;&#x6728;
  <pr>&#x304A;&#x308C;&#x3093;&#x3058;&#x306E;&#x304D;</pr></trd>
  <trdgrp lng="nl">
    <trd>sinaasappelboom</trd>,
    <trd>appelsienboom</trd>,
    <trd>oranjeboom</trd>
  </trdgrp>
  <trd lng="pl">pomara&nacute;cza  <klr>(krzew)</klr></trd>
  <trd lng="pt">laranjeira</trd>
  <trd lng="ro">portocala  <klr>(tufa)</klr></trd>
  <trd lng="ru">&c_a;&c_p;&c_je;&c_l;&c_mol;&c_s;&c_i;&c_n;&c_n;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  <trd lng="sv">apelsintr&auml;d</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43C;&c_a;&c_r;&c_a;&#x43D;&c_ch;&c_a;</trd>,
    <trd>&c_a;&#x43F;&c_je;&#x43B;&#x44C;&c_s;&c_i;&#x43D;(&#x43E;&c_v;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&#x43E;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6A58;&#x6811; <pr>j&#xFA;sh&#xF9;</pr></trd>,
    <trd>&#x6A58;&#x6A39; <pr>j&#xFA;sh&#xF9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="orangx.0kolora">
  <kap><tld/>kolora</kap>
  <snc>
    <dif>
      Havanta koloron mezan inter ru&gcirc;o kaj flavo; <ref tip="sin" cel="orangx.0a"><tld/>a</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_r;&c_a;&c_n;&c_zh;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_a;&c_r;&c_a;&c_n;&c_ch;&c_a;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="bo">&#xF63;&#xF72;&#xF0B;&#xF5D;&#xF44;&#xF0B;</trd>
  <trd lng="en">orange</trd>
  <trd lng="fy">oranje</trd>
  <trd lng="io">oranjea</trd>
  <trd lng="ja">&#x30AA;&#x30EC;&#x30F3;&#x30B8;&#x8272;&#x306E;
  <pr>&#x304A;&#x308C;&#x3093;&#x3058;&#x3044;&#x308D;&#x306E;</pr></trd>
  <trd lng="nl">oranje<klr tip="ind"> (bn.)</klr></trd>
  <trd lng="pl">pomara&nacute;czowy</trd>
  <trdgrp lng="ro">
    <trd>portocal</trd>,
    <trd>portocal&abreve;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43C;&c_a;&c_r;&c_a;&#x43D;&c_ch;&c_je;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_zh;&#x43E;&c_v;&c_t;&#x43E;&c_g;&c_a;&c_r;&#x44F;&c_ch;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7DF9; <pr>t&#xED;</pr></trd>,
    <trd>&#x7F07; <pr>t&#xED;</pr></trd>,
    <trd>&#x6A58;&#x7D05; <pr>j&#xFA;h&#xF3;ng</pr></trd>,
    <trd>&#x6A58;&#x7EA2; <pr>j&#xFA;h&#xF3;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="orangx.0koloro">
  <kap><tld/>koloro</kap>
  <snc>
    <dif>
      <ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">Koloro</ref> de <tld/>o, meza inter
      ru&gcirc;o kaj
      flavo.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_r;&c_a;&c_n;&c_zh;&c_a;&c_v;&c_y; &c_k;&c_o;&c_l;&c_je;&c_r;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_a;&c_r;&c_a;&c_n;&c_ch;&c_a;&c_v;&c_y; &c_k;&c_o;&c_l;&c_je;&c_r;</trd>
  </trdgrp>
  <trd lng="en">orange</trd>
  <trd lng="fy">oranje</trd>
  <trdgrp lng="id">
    <trd>jingga</trd>,
    <trd>oranye</trd>
  </trdgrp>
  <trd lng="ja">&#x30AA;&#x30EC;&#x30F3;&#x30B8;&#x8272; <pr>&#x304A;&#x308C;&#x3093;&#x3058;&#x3044;&#x308D;</pr></trd>
  <trd lng="nl">oranje<klr tip="ind"> (zn.)</klr></trd>
  <trdgrp lng="pl">
    <trd>pomara&nacute;czowy</trd>,
    <trd>oran&zdot;</trd>
  </trdgrp>
  <trd lng="ro">portocaliu</trd>
  <trdgrp lng="zh">
    <trd>&#x6A59; <pr>ch&#xE9;ng</pr></trd>,
    <trd>&#x6A58;&#x7D05; <pr>j&#xFA;h&#xF3;ng</pr></trd>,
    <trd>&#x6A58;&#x7EA2; <pr>j&#xFA;h&#xF3;ng</pr></trd>
  </trdgrp>
</drv>
</art>
