<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
<rad>Moskv</rad>/o
</kap>
<drv mrk="moskv.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="urb.0o">Urbo</ref> en
      <ref tip="malprt" cel="rus.R0ujo">Rusujo</ref>
      (37&ring;37'E, 55&ring;44'N),
      anta&ubreve; 1712 kaj ekde 1918 la &ccirc;efurbo de Rusujo, kaj de 1922 &gcirc;is
      1991 anka&ubreve; de <ref tip="malprt"
      cel="sovet.S0unio">Sovetunio</ref>:
      <ekz>
        Foje en <tld/>o, dum malnormale varmega printempa sunsubiro, du
        civitanoj venis la &gcirc;ardenplacon de la Patriar&hcirc;a lageto.
        <fnt><bib>MkM</bib>, <lok></lok></fnt>
      </ekz>
      <ekz>
        La gotojn portis ega fortostre&ccirc;o /
        sub Moskvon, kaj, jubile pri l' atingoj, /
        ili sin pu&scirc;is plu en duonringoj, /
        por rabi tiun urbon per sie&gcirc;o.
       <fnt>
         <aut>K. Kalocsay</aut>:
         <vrk>Sub Moskvo</vrk>.
       </fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="peterburg.0o">Peterburgo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_M;&c_a;&c_s;&c_k;&c_v;&c_a;</trd>
  <trd lng="bg">&c_M;&c_o;&c_s;&c_k;&c_v;&c_a;</trd>
  <trd lng="de">Moskau</trd>
  <trd lng="en">Moscow</trd>
  <trd lng="es">Mosc&uacute;</trd>
  <trd lng="fr">Moscou</trd>
  <trd lng="hu">Moszkva</trd>
  <trd lng="nl">Moskou</trd>
  <trd lng="os">&c_M;&aelig;&c_s;&c_k;&c_u;&c_y;</trd>
  <trdgrp lng="pt">
   <trd>Moscou</trd>,
    <trd>Moscovo <klr tip="amb">(PT)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_M;&c_o;&c_s;&c_k;&c_v;&c_a;</trd>
  <trd lng="sk">Moskva <klr>(mesto)</klr></trd>
  <trd lng="sv">Moskva</trd>
  <trd lng="zh">&#33707;&#26031;&#31185;<klr> [M&ograve;sike]</klr></trd>
  <trd lng="pt">Moscovo</trd>
</drv>
<drv mrk="moskv.0orivero">
  <kap><tld/>o-rivero</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="river.0o">Rivero</ref>, kiu fluas tra
      urbo <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_M;&c_a;&c_s;&c_k;&c_v;&c_a;-&c_r;&c_a;&c_k;&c_a;</trd>
  <trd lng="de">Moskwa</trd>
  <trd lng="hu">Moszkva-foly&oacute;</trd>
  <trd lng="nl">Moskou-rivier</trd>
  <trdgrp lng="pt">
   <trd>Moscova</trd>,
    <trd>M&oacute;scova</trd>
  </trdgrp>
  <trd lng="ru">&c_M;&c_o;&c_s;&c_k;&c_v;&c_a;-&c_r;&c_je;&c_k;&c_a;</trd>
  <trd lng="sk">Moskva <klr>(rieka)</klr></trd>
</drv>
</art>
