  </snc>
  <trd lng="be">&c_v;&c_y;&c_r;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">in <ind>Sicherheit</ind> bringen</trd>
