<drv mrk="sav.0o">
  <kap><tld/>o</kap>
  <snc mrk="sav.0o.liberigo">
    <dif>
      Liberigo el grava dan&gcirc;ero:
      <ekz>
        al sklavo mon' ne estas <tld/>o &#8208; li &ccirc;iam restas sklavo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne ekzistas <tld/>o kontra&ubreve; malbona virino
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de judaj tombejoj en E&ubreve;ropo
        <fnt>
	<aut>Ji&rcaron;i Patera</aut>
	<vrk><url ref="&Monato;/2001/007207.php">La mezepokaj judaj mortintoj fine
          trankvilaj</url></vrk>
	<bib>Monato</bib>
	<lok>2001/01, p. 14</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de konkuranto
        <fnt>
          <aut>Roland Rotsaert</aut>
            <vrk><url ref="&Monato;/2000/007217.php">Vindozo kaj Linukso
            interproksimi&gcirc;as</url></vrk>
            <bib>Monato</bib>
            <lok>2000/12, p. 17</lok>
          </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>okuracisto</ind>
        <fnt>
          <aut>Stefan Maul</aut>
            <vrk><url ref="&Monato;/2000/007190.html">Trompitaj dizertintoj</url></vrk>
            <bib>Monato</bib>
            <lok>2000/12, p. 9</lok>
          </fnt>;
        <trd lng="de">Notarzt</trd>
      </ekz>
      <ekz>
        <ref cel="boat.sav0o">savboato</ref>
        <fnt>
          <bib>Monato</bib>
          <aut>Christian Bertin</aut>
          <vrk><url ref="&Monato;/2004/008255.html">Re&gcirc;ino de la maroj</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu banejo devas nepre disponi pri <ind><tld/>ringo</ind> kaj &jcirc;etebla <tld/>&scirc;nurego
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2009/009629.php">Vintra banado</url></vrk>
          <lok>2009</lok>
        </fnt>.
        <trd lng="de">Rettungsring</trd>
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>rescat</trd>,
      <trd>salvament</trd>
    </trdgrp>
    <trd lng="de">Rettung</trd>
    <trd lng="hu">megment&eacute;s</trd>
    <trdgrp lng="it">
     <trd>salute<klr> (salvezza)</klr></trd>,
     <trd>salvezza</trd>
     </trdgrp>
    <trdgrp lng="pl">
      <trd>ratunek</trd>,
      <trd>wybawienie</trd>,
      <trd>ocalenie</trd>
    </trdgrp>
    <trd lng="vo">sav</trd>
  </snc>
  <snc mrk="sav.0o.postmorta">
    <dif>
      Postmorta eterna feli&ccirc;o:
      <ekz>
        proklamu de tago al tago Lian <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>I. Kroniko 16:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        la universala <tld/>o farita de Kristo
        <fnt>
          <aut>Antonio Capello</aut>
            <vrk><url ref="&Monato;/2001/007300.php">Antisemitismo kaj antijudismo</url></vrk>
            <bib>Monato</bib>
            <lok>2001/02, p. 4</lok>
          </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tabul.0o">tabulo</ref>,
      <ref cel="arme.sav0o">savarmeo</ref>.
    </refgrp>
    <trd lng="ca">salvaci&oacute;</trd>
    <trd lng="de">Heil</trd>
    <trd lng="hu">&uuml;dv&ouml;z&uuml;l&eacute;s</trd>
    <trd lng="it">salvezza</trd>
    <trd lng="pl">zbawienie</trd>
    <trd lng="vo">san</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_t;&c_u;&c_n;&c_a;&c_k;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_a;&c_t;&c_u;&c_n;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>osvobozen&iacute;</trd>,
    <trd>sp&aacute;sa</trd>,
    <trd>z&aacute;chrana</trd>
  </trdgrp>
  <trd lng="de">Rettung</trd>
  <trd lng="es">salvaci&oacute;n</trd>
  <trd lng="fr">salut</trd>
  <trdgrp lng="ja">
    <trd>&#25937;&#12356;  <pr>&#12377;&#12367;&#12356;</pr></trd>,
    <trd>&#25937;&#21161;  <pr>&#12365;&#12421;&#12358;&#12376;&#12423;</pr></trd>,
    <trd>&#25937;&#28168;  <pr>&#12367;&#12373;&#12356;</pr></trd>,
    <trd>&#28168;&#24230;  <pr>&#12373;&#12356;&#12393;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>redding</trd>,
    <trd>heil</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_p;&c_a;&c_s;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>oslobodenie</trd>,
    <trd>sp&aacute;sa</trd>,
    <trd>z&aacute;chrana</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_ja;&c_t;&c_u;&c_n;&c_o;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_ja;&c_t;&c_u;&c_n;&c_o;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="sav.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="sav.0ado.ago">
    <dif>
      Ago <tld/>i:
      <ekz>
        la <tld/>ado de sia domo
        <fnt>
          <bib>NT</bib>
          <lok>Hebreoj 11:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        la franca societo de <tld/>ado de dronantoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#8230;]</klr> la&ubreve;dis la <tld/>adon de la Komodo-varanoj
       <fnt>
          <aut>Wim de Smet</aut>
          <vrk><url ref="&Monato;/2000/006900.php">La &leftquot;drakoj&#8221; de Komodo:
          Gigantaj bestoj sur izola insulo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 18</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
<trd lng="be">&c_r;&c_a;&c_t;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="de">Rettung</trd>
  <trd lng="fr">sauvetage</trd>
  <trd lng="ja">&#25937;&#21161;  <pr>&#12365;&#12421;&#12358;&#12376;&#12423;</pr></trd>
  <trdgrp lng="pl">
    <trd>ratowanie</trd>,
    <trd>ochranianie</trd>
  </trdgrp>
  <trd lng="uk">&c_r;&c_ja;&c_t;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>
</drv>

<drv mrk="sav.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sav.0igxi.KOMUNE">
    <dif>
      Esti <tld/>ita (eventuale per sia propra ago),
      <ref tip="vid" cel="eskap.0i">eskapi</ref> el
      dan&gcirc;ero:
      <ekz>
        <nom>David</nom> foriris de tie, kaj <tld/>i&gcirc;is en la kaverno
        <fnt><bib>MT</bib>, <lok>&Sa1;, 22:1</lok></fnt>;
      </ekz>
      <ekz>
        oni <klr>[&#8230;]</klr> ne konstruis tiun centralon, kaj <tld/>i&gcirc;is valora apudrivera
        naturprotektita arbaro
        <fnt><aut>Walter Klag</aut>,
        <vrk><url ref="&Monato;/2000/006938.php">Friedensreich Hundertwasser 1928 -
        2000</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/07, p. 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &gcirc;usta vojo al <tld/>i&gcirc;o
        <fnt>
          <aut>Ji&rcaron;&iacute; Patera</aut>
            <vrk><url ref="&Monato;/2000/007031.php">Johano Hus &ndash; &ccirc;u ne plu herezulo?</url></vrk>
            <bib>Monato</bib>
            <lok>2000/08, p. 19</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
