<drv mrk="sav.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="sav.0ado.ago">
    <dif>
      Ago <tld/>i:
      <ekz>
        la <tld/>ado de sia domo
        <fnt>
          <bib>NT</bib>
          <lok>Hebreoj 11:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        la franca societo de <tld/>ado de dronantoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#x2026;]</klr> la&ubreve;dis la <tld/>adon de la Komodo-varanoj
       <fnt>
          <aut>Wim de Smet</aut>
          <vrk><url ref="&Monato;/2000/006900.php">La &leftquot;drakoj&#x201D; de Komodo:
          Gigantaj bestoj sur izola insulo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 18</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
<trd lng="be">&c_r;&c_a;&c_t;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
