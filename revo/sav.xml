<drv mrk="sav.0o">
  <kap><tld/>o</kap>
  <snc mrk="sav.0o.liberigo">
    <dif>
      Liberigo el grava dan&gcirc;ero:
      <ekz>
        al sklavo mon' ne estas <tld/>o &#x2010; li &ccirc;iam restas sklavo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne ekzistas <tld/>o kontra&ubreve; malbona virino
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de judaj tombejoj en E&ubreve;ropo
        <fnt>
	<aut>Ji&rcaron;i Patera</aut>
	<vrk><url ref="&Monato;/2001/007207.php">La mezepokaj judaj mortintoj fine
          trankvilaj</url></vrk>
	<bib>Monato</bib>
	<lok>2001/01, p. 14</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de konkuranto
        <fnt>
          <aut>Roland Rotsaert</aut>
            <vrk><url ref="&Monato;/2000/007217.php">Vindozo kaj Linukso
            interproksimi&gcirc;as</url></vrk>
            <bib>Monato</bib>
            <lok>2000/12, p. 17</lok>
          </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>okuracisto</ind>
        <fnt>
          <aut>Stefan Maul</aut>
            <vrk><url ref="&Monato;/2000/007190.html">Trompitaj dizertintoj</url></vrk>
            <bib>Monato</bib>
            <lok>2000/12, p. 9</lok>
          </fnt>;
        <trd lng="de">Notarzt</trd>
      </ekz>
      <ekz>
        <ref cel="boat.sav0o">savboato</ref>
        <fnt>
          <bib>Monato</bib>
          <aut>Christian Bertin</aut>
          <vrk><url ref="&Monato;/2004/008255.html">Re&gcirc;ino de la maroj</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu banejo devas nepre disponi pri <ind><tld/>ringo</ind> kaj &jcirc;etebla <tld/>&scirc;nurego
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2009/009629.php">Vintra banado</url></vrk>
          <lok>2009</lok>
        </fnt>.
        <trd lng="de">Rettungsring</trd>
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>rescat</trd>,
      <trd>salvament</trd>
    </trdgrp>
    <trd lng="de">Rettung</trd>
    <trd lng="hu">megment&eacute;s</trd>
    <trdgrp lng="it">
     <trd>salute<klr> (salvezza)</klr></trd>,
     <trd>salvezza</trd>
     </trdgrp>
    <trdgrp lng="pl">
      <trd>ratunek</trd>,
      <trd>wybawienie</trd>,
      <trd>ocalenie</trd>
    </trdgrp>
    <trd lng="vo">sav</trd>
  </snc>
  <snc mrk="sav.0o.postmorta">
    <dif>
      Postmorta eterna feli&ccirc;o:
      <ekz>
        proklamu de tago al tago Lian <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>I. Kroniko 16:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        la universala <tld/>o farita de Kristo
        <fnt>
          <aut>Antonio Capello</aut>
            <vrk><url ref="&Monato;/2001/007300.php">Antisemitismo kaj antijudismo</url></vrk>
            <bib>Monato</bib>
            <lok>2001/02, p. 4</lok>
          </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tabul.0o">tabulo</ref>,
      <ref cel="arme.sav0o">savarmeo</ref>.
    </refgrp>
    <trd lng="ca">salvaci&oacute;</trd>
    <trd lng="de">Heil</trd>
    <trd lng="hu">&uuml;dv&ouml;z&uuml;l&eacute;s</trd>
    <trd lng="it">salvezza</trd>
    <trd lng="pl">zbawienie</trd>
    <trd lng="vo">san</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_t;&c_u;&c_n;&c_a;&c_k;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_a;&c_t;&c_u;&c_n;&c_a;&c_k;</trd>
  </trdgrp>
