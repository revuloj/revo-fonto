  <trd lng="it">salvare</trd>
  <trd lng="ja">&#x4FDD;&#x5B58;</trd>
  <trd lng="jw">nyimpen</trd>
  <trd lng="ka">&#x10D2;&#x10D0;&#x10D3;&#x10D0;&#x10E0;&#x10E9;&#x10D4;&#x10DC;&#x10D0;</trd>
  <trd lng="km">&#x179A;&#x1780;&#x17D2;&#x179F;&#x17B6;&#x1791;&#x17BB;&#x1780;</trd>
