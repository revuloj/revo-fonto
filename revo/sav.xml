    <trd>&c_z;&c_a;&c_h;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bn">&#x9B8;&#x982;&#x9B0;&#x995;&#x9CD;&#x9B7;&#x9A3;</trd>
  <trd lng="bo">&#xF66;&#xF90;&#xFB1;&#xF7C;&#xF56;&#xF0B;&#xF54;&#xF0B;</trd>
  <trd lng="bs">spasiti</trd>
  <trd lng="ca">salvar</trd>
  <trd lng="co">sarvari</trd>
  <trdgrp lng="cs">
    <trd>uchovat</trd>,
    <trd>ulo&zcaron;it</trd>,
    <trd>vkl&aacute;dat</trd>,
    <trd>zapsat do pam&ecaron;ti</trd>
  </trdgrp>
  <trd lng="da">spare</trd>
  <trd lng="de">retten</trd>
  <trd lng="en">save</trd>
  <trd lng="es">salvar</trd>
  <trd lng="et">s&#xE4;&#xE4;sta</trd>
  <trd lng="eu">salbatu</trd>
  <trd lng="fr">sauver</trd>
  <trd lng="fy">r&#xEA;de</trd>
  <trd lng="gl">gardar</trd>
  <trd lng="gu">&#xAB8;&#xAC7;&#xAB5;</trd>
  <trd lng="ha">ajiye</trd>
  <trd lng="hi">&#x92C;&#x91A;&#x93E;</trd>
  <trd lng="hr">spasiti</trd>
  <trd lng="hu">megment</trd>
  <trd lng="hy">&#x57A;&#x561;&#x570;&#x565;&#x56C;</trd>
