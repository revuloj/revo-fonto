<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sav.xml,v 1.25 2020/02/06 00:10:32 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sav</rad>/i
</kap>
<drv mrk="sav.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sav.0i.liberigi">
    <dif>
      <ref cel="liber.0igi">Liberigi</ref> iun el morta a&ubreve; grava dan&gcirc;ero:
      <ekz>
        longa konsidero <tld/>as de sufero
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        kiu sin gardas, tiu sin <tld/>as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        Li <tld/>u de morto ilian animon
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 33:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        prudento vin gardos <klr>[&#8230;]</klr> por <tld/>i vin de fremda <klr>[&#8230;]</klr>
        edzino
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 2:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u min de &ccirc;ia mizero
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 26:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[Li]</klr> <tld/>is vin el la manoj de la Egiptoj
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 18:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u nin kontra&ubreve; liaj manoj
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 37:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> <tld/>is sin <klr>(forkurante)</klr> sur &ccirc;evalo
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 20:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la monton <tld/>u vin <klr>(forkuru)</klr>, por ke vi ne pereu
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 19:17</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>ratowa&cacute;</trd>,
      <trd>wybawia&cacute;</trd>,
      <trd>ocala&cacute;</trd>,
      <trd>zbawia&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sav.0i.gardi">
    <dif>
      <ref cel="gard.0i">Gardi</ref> ion de difekto a&ubreve; pereo:
      <ekz>
        &ccirc;io, kio restis <tld/>ita <klr>[&#8230;]</klr> kontra&ubreve; la hajlo
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 10:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        dank' al tio la entrepreno povis esti <tld/>ata el absoluta
        malsukceso;
      </ekz>
      <ekz>
        la manovro <klr>[&#8230;]</klr> <tld/>is la situacion
        <fnt>
          <bib>Monato</bib>
          <aut>Marc Vanden Bempt kaj Paul Peeraerts</aut>
          <vrk><url ref="https://www.monato.be/2009/009714.php"
              >Savintoj ne savitaj</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i <klr>(konservi netu&scirc;ita)</klr> la dignon;
      </ekz>
      <ekz>
        <tld/>i sian animon
        <klr>(certigi al si postmortan feli&ccirc;on)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konser.0i.protekti">konservi<sncref/></ref>,
      <ref cel="protek.0i.sxirmi">protekti<sncref/></ref>,
      <ref cel="gard.0i.protekti">gardi<sncref/></ref>
    </refgrp>
    <trdgrp lng="fr">
      <trd>pr&eacute;server</trd>,
      <trd>sauvegarder</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>ratowa&cacute;</trd>,
      <trd>ocala&cacute;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_r;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_h;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">salvar</trd>
  <trdgrp lng="cs">
    <trd>uchovat</trd>,
    <trd>ulo&zcaron;it</trd>,
    <trd>vkl&aacute;dat</trd>,
    <trd>zapsat do pam&ecaron;ti</trd>
  </trdgrp>
  <trd lng="de">retten</trd>
  <trd lng="en">save</trd>
  <trd lng="es">salvar</trd>
  <trd lng="eu">salbatu</trd>
  <trd lng="fr">sauver</trd>
  <trd lng="hu">megment</trd>
  <trd lng="it">salvare</trd>
  <trd lng="nl">redden</trd>
  <trd lng="pt">salvar</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_a;&c_s;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_s;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ulo&zcaron;i&tcaron;</trd>,
    <trd>zap&iacute;sa&tcaron; do pam&auml;te</trd>
  </trdgrp>
  <trd lng="vo">sav&ouml;n</trd>
</drv>

<drv mrk="sav.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Sendan&gcirc;erigita vivo.
    </dif>
    <trdgrp lng="ca">
      <trd>rescat</trd>,
      <trd>salvament</trd>
    </trdgrp>
    <trd lng="de">Rettung</trd>
    <trd lng="hu">megment&eacute;s</trd>
    <trdgrp lng="it">
     <trd>salute<klr> (salvezza)</klr></trd>, 
     <trd>salvezza</trd>
     </trdgrp>
    <trdgrp lng="pl">
      <trd>ratunek</trd>,
      <trd>wybawienie</trd>,
      <trd>ocalenie</trd>
    </trdgrp>
    <trd lng="vo">sav</trd>
  </snc>
  <snc>
    <dif>
      Postmorta eterna feli&ccirc;o:
      <ekz>
        ekster la eklezio ne estas <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tabul.0o">tabulo</ref>,
      <ref cel="arme.sav0o">savarmeo</ref>.
    </refgrp>
    <trd lng="ca">salvaci&oacute;</trd>
    <trd lng="de">Heil</trd>
    <trd lng="hu">&uuml;dv&ouml;z&uuml;l&eacute;s</trd>
    <trd lng="it">salvezza</trd>
    <trd lng="pl">zbawienie</trd>
    <trd lng="vo">san</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_t;&c_u;&c_n;&c_a;&c_k;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_a;&c_t;&c_u;&c_n;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>osvobozen&iacute;</trd>,
    <trd>sp&aacute;sa</trd>,
    <trd>z&aacute;chrana</trd>
  </trdgrp>
  <trd lng="es">salvaci&oacute;n</trd>
  <trd lng="fr">salut</trd>
  <trdgrp lng="nl">
    <trd>redding</trd>,
    <trd>heil</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_p;&c_a;&c_s;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>oslobodenie</trd>,
    <trd>sp&aacute;sa</trd>,
    <trd>z&aacute;chrana</trd>
  </trdgrp>
</drv>

<drv mrk="sav.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <trd lng="fr">sauvetage</trd>
</drv>

<drv mrk="sav.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sav.0igxi.KOMUNE">
    <dif>
      <tld lit="S"/>i sin mem,
      <ref tip="vid" cel="eskap.0i">eskapi</ref> el
      dan&gcirc;ero:
      <ekz>
        <nom>David</nom> foriris de tie, kaj <tld/>i&gcirc;is en la kaverno
        <fnt><bib>MT</bib>, <lok>&Sa1;, 22:1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">se <ind>sauver</ind></trd>
</drv>

<drv mrk="sav.antaux0i">
  <kap>anta&ubreve;<tld/>i</kap>
  <snc mrk="sav.antaux0i.KOMUNE">
    <dif>
      Anticipe <tld/>i de eventuala dan&gcirc;ero:
      <ekz>
        mi tion faros kun plena neka&scirc;emeco pro tio, ke mi
        anta&ubreve;<tld/>u vin de novaj trompi&gcirc;oj kaj disrevi&gcirc;oj
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="gard.antaux0i">anta&ubreve;gardi</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_s;&c_mol;&c_c;&c_je;&c_r;&c_a;&c_g;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_mol;&c_c;&c_je;&c_r;&c_a;&c_g;&c_ch;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>prevenir</trd>,
    <trd>preservar</trd>,
    <trd>premunir</trd>
  </trdgrp>
  <trd lng="fr">pr&eacute;server</trd>
  <trd lng="it">preservare<klr> (salvezza)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_d;&c_o;&c_s;&c_t;&c_je;&c_r;&c_je;&c_ch;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_o;&c_h;&c_r;&c_a;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
