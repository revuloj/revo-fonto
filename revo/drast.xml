<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>drast</rad>/a
<fnt>Z</fnt></kap>
<drv mrk="drast.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Fortege efikanta <klr>(ekz-e p.p. kuracilo)</klr>.
    </dif>
    <trdgrp lng="be">
      <trd>&c_m;&c_o;&c_c;&c_n;&c_y;</trd>,
      <trd>&c_m;&c_o;&c_c;&c_n;&c_a;&c_d;&c_z;&c_je;&c_ju;&c_ch;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>wirksam</trd>,
      <trd>stark</trd>
    </trdgrp>
    <trd lng="fr">puissant<klr tip="ind"> (tr&egrave;s efficace)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_i;&c_l;&c_mol;&c_n;&c_o;&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_u;&c_ju;&c_shch;&c_i;&c_j;</trd>,
      <trd>&c_r;&c_je;&c_z;&c_k;&c_i;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fortege impresanta, produktanta fortan efekton:
      <ekz>
        <tld/>a esprimo, <tld/>a sceno
      </ekz>
      .
    </dif>
    <refgrp tip="sin">
      <ref cel="energi.0a">energia</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_r;&c_e;&c_z;&c_k;&c_ib;</trd>,
      <trd>&c_e;&c_f;&c_e;&c_k;&c_t;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>drastisch</trd>,
      <trd>gut einschlagend</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>impressionnant</trd>,
      <trd>frappant</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_l;&c_ja;&c_ju;&c_shch;&c_i;&c_j;</trd>,
      <trd>&c_e;&c_f;&c_f;&c_je;&c_k;&c_t;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_r;&c_je;&c_z;&c_k;&c_i;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="nl">drastisch</trd>
</drv>
</art>
