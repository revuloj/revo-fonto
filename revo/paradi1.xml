<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc><rad>paradiz</rad>/o</kap>
<drv mrk="paradi1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Bela &gcirc;ardeno, en kiu, la&ubreve; la Biblio, vivis Adam kaj Eva
      anta&ubreve; la
      peko.
    </dif>
    <trdgrp lng="en">
      <trd>Eden</trd>,
      <trd>Garden of <ind>Eden</ind></trd>,
      <trd>Paradise</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>paradicsom</trd>,
      <trd>&eacute;denkert</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Loko &ccirc;iela, kie justuloj &gcirc;uas post morto eternan
      feli&ccirc;on.
    </dif>
    <trdgrp lng="en">
      <trd>Heaven</trd>,
      <trd>Paradise</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>paradicsom</trd>,
      <trd>mennyorsz&aacute;g</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plej agrabla loko.
    </dif>
    <refgrp tip="sin">
      <ref cel="eden.0o">edeno</ref>.
    </refgrp>
    <trdgrp lng="en">
      <trd>paradise</trd>,
      <trd>Eden</trd>
    </trdgrp>
    <trd lng="hu">f&ouml;ldi paradicsom</trd>    
  </snc>                   
  <trd lng="be">&c_r;&c_a;&c_j;</trd>
  <trd lng="br">baradoz</trd>
  <trd lng="de">Paradies</trd>
  <trd lng="fr">paradis</trd>
  <trd lng="nl">paradijs</trd>
  <trd lng="ru">&c_r;&c_a;&c_j;</trd>
  <trd lng="sv">paradis</trd>
</drv>

<drv mrk="paradi1.lud0o">
  <kap><tld/>ludo</kap>
  <snc>
    <dif>
      <ref tip="super" cel="lud.0o">Ludo</ref> en kiu oni devas, starante
      sur unu piedo, pu&scirc;i &scirc;toneton tra la fakoj de surtera
      desegnita
      plano &gcirc;is la lasta nomata paradizo:
      <ekz>
        mi rimarkigis al &scirc;i, ke la plankokaheloj memorigas min pri la
        <tld/>ludo, kaj tiel ekis konversacio pri infanludoj.
        <fnt><aut>Garbhan MACAOIDH</aut>:
        <vrk><url ref="&Monato;/007517.html">Perdita paradizo</url></vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 10/2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>                           
  <trd lng="br">troadig-kamm</trd>
  <trd lng="de">Himmelhoppe</trd>
  <trd lng="en">hopscotch</trd>
  <trd lng="es">coxcojilla</trd>
  <trd lng="fr">marelle</trd>
  <trd lng="hu">ugr&oacute;iskola</trd>  
  <trd lng="nl">hinkelspel</trd>
  <trdgrp lng="pt">
   <trd>amarelinha</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_l;&c_a;&c_s;&c_s;&c_i;&c_k;&c_i; <klr
  tip="amb">(&c_i;&c_g;&c_r;&c_a;)</klr></trd>
</drv>
</art>
