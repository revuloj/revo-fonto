<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>rubus</rad>/o
</kap>
<drv mrk="rubus.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="rubus.0o.genro">
    <dif>
      Genro el familio <ref tip="super" cel="roz.0acoj">rozacoj</ref>
      (<trd lng="la">Rubus</trd>),
      dorna arbedo kun ru&gcirc;aj a&ubreve;
      nigraj, dol&ccirc;aj, man&gcirc;eblaj fruktoj.
      Unu &gcirc;ia specio estas la
      <ref tip="sub" cel="framb.0ujo">frambujo</ref>.
    </dif>
  </snc>
  <snc mrk="rubus.0o.specio">
    <dif>
      Specio de <ref tip="super" cel="rubus.0o.genro"><tld/>o
      <sncref ref="rubus.0o.genro"/></ref>
      (<trd lng="la">Rubus fruticosus</trd>), kun nigraj beroj.
    </dif>
    <bld lok="../bld/rubus.jpg">floroj kaj folioj de nigra <tld/>o</bld>
    <trd lng="de">Brombeere</trd>
    <trd lng="fr">m&ucirc;re</trd>
    <trd lng="nl">braam <klr tip="ind">(vrucht)</klr></trd>
  </snc>
</drv>
</art>
