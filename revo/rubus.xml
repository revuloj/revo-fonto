<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>rubus</rad>/o
</kap>
<drv mrk="rubus.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="rubus.0o.genro">
    <dif>
      Genro el familio <ref tip="super" cel="rozac.0oj">rozacoj</ref>
      (<trd lng="la">Rubus</trd>), dorna arbedo kun ru&gcirc;aj a&ubreve;
      nigraj, dol&ccirc;aj, man&gcirc;eblaj plurfruktoj.
      Unu &gcirc;ia specio estas la
      <ref tip="sub" cel="framb.0ujo">frambujo</ref>.
    </dif>
    <trd lng="nl">braam<klr tip="ind"> (vruchten)</klr></trd>
    <trd lng="ru">&c_r;&c_u;&c_b;&c_u;&c_s; <klr tip="amb">(&c_r;&c_o;&c_d; &c_r;&c_o;&c_z;&c_o;&c_c;&c_v;&c_je;&c_t;&c_n;&c_y;&c_h; &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_j;)</klr></trd>
  </snc>
  <snc mrk="rubus.0o.specio">
    <dif>
      Specio de <ref tip="super" cel="rubus.0o.genro"><tld/>o
      <sncref ref="rubus.0o.genro"/></ref>
      (<trd lng="la">Rubus fruticosus</trd>), kun nigra plurfrukto.
    </dif>
    <bld lok="../bld/rubus.jpg">
      floroj kaj folioj de nigra <tld/>o
    </bld>
    <trd lng="de">Brombeere</trd>
    <trd lng="fr">m&ucirc;re</trd>
    <trd lng="nl">braambes</trd>
    <trd lng="ru">&c_je;&c_zh;&c_je;&c_v;&c_i;&c_k;&c_a;</trd>
  </snc>
</drv>
</art>
