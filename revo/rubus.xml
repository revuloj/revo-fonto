<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>rubus</rad>/o
</kap>
<drv mrk="rubus.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="rubus.0o.genro">
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="rozac.0oj">rozacoj</ref>
      (<trd lng="la">Rubus</trd>), dorna arbedo kun ru&gcirc;aj a&ubreve;
      nigraj, dol&ccirc;aj, man&gcirc;eblaj plurfruktoj.
      Unu &gcirc;ia specio estas la
      <ref tip="sub" cel="framb.0ujo">frambujo</ref>.
    </dif>
    <trdgrp lng="hu">
      <trd>szeder</trd>,
      <trd>m&aacute;lna</trd>
    </trdgrp>
    <trd lng="nl">braam<klr tip="ind"> (vruchten)</klr></trd>
    <trdgrp lng="pl">
      <trd>je&zdot;yna</trd>,
      <trd>o&zdot;yna</trd>,
      <trd>ostr&eogonek;&zdot;yna</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>silva</trd>,
      <trd>sar&ccedil;a</trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_u;&c_b;&c_u;&c_s;
       <klr tip="amb">(&c_r;&c_o;&c_d; &c_r;&c_o;&c_z;&c_o;&c_c;&c_v;&c_je;&c_t;&c_n;&c_y;&c_h; &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_j;)</klr></trd>
  </snc>
  <snc mrk="rubus.0o.specio">
    <dif>
      <ref tip="lst" cel="speci.0o.BIO"
      lst="voko:botanikaj_specioj">Specio</ref> de <ref tip="malprt"
      cel="rubus.0o.genro"><tld/>o
      <sncref ref="rubus.0o.genro"/></ref>
      (<trd lng="la">Rubus fruticosus</trd>), kun nigra plurfrukto.
    </dif>
    <bld lok="../bld/rubus.jpg">
      floroj kaj folioj de nigra <tld/>o
    </bld>
    <trd lng="be">&c_a;&c_zh;&c_y;&c_n;&c_a;</trd>
    <trd lng="bg">&c_k;&c_malmol;&c_p;&c_i;&c_n;&c_a;</trd>
    <trd lng="de">Brombeere</trd>
    <trd lng="fr">m&ucirc;re</trd>
    <trdgrp lng="hu">
      <trd>vadszeder</trd>,
      <trd>f&ouml;ldi <ind>szeder</ind></trd>,
      <trd>fekete <ind>szeder</ind></trd>
    </trdgrp>
    <trd lng="nl">braambes</trd>
    <trd lng="pl">je&zdot;yna krzewiasta</trd>
    <trd lng="ru">&c_je;&c_zh;&c_je;&c_v;&c_i;&c_k;&c_a;</trd>
  </snc>
  <trdgrp lng="en">
    <trd>blackberry</trd>,
    <trd>bramble</trd>
  </trdgrp>
</drv>
</art>
