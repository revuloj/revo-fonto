<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>rubus</rad>/o
</kap>
<drv mrk="rubus.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="rubus.0o.genro">
    <dif>
      Genro el familio <ref tip="super" cel="rozac.0oj">rozacoj</ref>
      (<trd lng="la">Rubus</trd>), dorna arbedo kun ruĝaj aŭ
      nigraj, dolĉaj, manĝeblaj plurfruktoj.
      Unu ĝia specio estas la
      <ref tip="sub" cel="framb.0ujo">frambujo</ref>.
    </dif>
    <trdgrp lng="hu">
      <trd>szeder</trd>,
      <trd>málna</trd>
    </trdgrp>
    <trd lng="nl">braam<klr tip="ind"> (vruchten)</klr></trd>
  <trdgrp lng="pt">
    <trd>silva</trd>,
    <trd>sarça</trd>
  </trdgrp>
    <trd lng="ru">рубус <klr tip="amb">(род
розоцветных
растений)</klr></trd>
  </snc>
  <snc mrk="rubus.0o.specio">
    <dif>
      Specio de <ref tip="super" cel="rubus.0o.genro"><tld/>o
      <sncref ref="rubus.0o.genro"/></ref>
      (<trd lng="la">Rubus fruticosus</trd>), kun nigra plurfrukto.
    </dif>
    <bld lok="../bld/rubus.jpg">
      floroj kaj folioj de nigra <tld/>o
    </bld>
    <trd lng="be">ажына</trd>
    <trd lng="de">Brombeere</trd>
    <trd lng="fr">mûre</trd>
    <trdgrp lng="hu">
      <trd>vadszeder</trd>,
      <trd>földi <ind>szeder</ind></trd>,
      <trd>fekete <ind>szeder</ind></trd>
    </trdgrp>
    <trd lng="nl">braambes</trd>
    <trd lng="ru">ежевика</trd>
  </snc>
  <trd lng="en">blackberry</trd>
</drv>
</art>
