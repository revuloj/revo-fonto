<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>*</ofc><rad>m</rad> <fnt><bib>PIV1</bib></fnt>
</kap>
<subart>
<drv mrk="m.0.konsonanto">
  <kap><tld/></kap>
    <uzo tip="fak">TIP</uzo>
    <uzo tip="fak">GRA</uzo>
    <uzo tip="fak">FON</uzo>
  <snc>    
    <dif>
      <ref tip="dif" cel="konson1.0o">Konsonanto</ref>, vo&ccirc;a <ref
      tip="dif" cel="nazal.0o">nazala</ref> <ref tip="dif"
      cel="labial.0o">labialo</ref> [m].
    </dif>
  <trd lng="fr">m<klr> (consonne)</klr></trd>  
  </snc>
  <snc>
     <dif>
      La deksepa <ref tip="dif" cel="grafem.0o">grafemo</ref> de la
      Esperanta alfabeto, uzata i.a. kiel:
     </dif>
  <trd lng="fr">m<klr> (lettre)</klr></trd>
  <subsnc>
  <uzo tip="fak">MAT</uzo>
  <dif>
  (maj.) romana cifero: <ekz><tld lit="M"/> (1000)</ekz>;
  </dif>
  <trd lng="fr">1000<klr> (chiffre romain)</klr></trd>
  </subsnc>
  <subsnc>
  <uzo tip="fak">FIZ</uzo>
  <dif>
  (maj.) simbolo de mega: <ekz><tld lit="M"/></ekz>;
  </dif>
  <trd lng="fr">m&eacute;ga</trd>
  </subsnc>
  <subsnc>
  <uzo tip="fak">FIZ</uzo>
  <dif>
  (min.) simbolo de metro: <ekz><tld/></ekz>;
  </dif>
  <trd lng="fr">m&egrave;tre</trd>
  </subsnc>
  <subsnc>
  <uzo tip="fak">FIZ</uzo>
  <dif>
  (min.) simbolo de mili: <ekz><tld/><tld/> (milimetro)</ekz>;
  </dif>
  <trd lng="fr">mili</trd>
  </subsnc>
  
  </snc>
  </drv>
</subart>

<subart>
<drv mrk="m.0.nomo">
  <kap><tld/>o</kap>
  <snc>  
    <dif>
      Nomo de tiuj <ref tip="dif" cel="fonem.0o">fonemo</ref> kaj
      grafemo.    
    </dif>
  <trd lng="fr">m</trd>
  </snc>
</drv>
</subart>
</art>

