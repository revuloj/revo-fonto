<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: m.xml,v 1.11 2023/09/02 06:04:15 revo Exp $">
<kap>
    <ofc>*</ofc> <ofc>7</ofc><rad>mo</rad>
</kap>

<drv mrk="m.0">
  <kap><tld/></kap>
    <uzo tip="fak">LIN</uzo>
    <uzo tip="fak">GRA</uzo>
    <uzo tip="fak">FON</uzo>
  <snc mrk="m.0.konsonanto">    
    <dif>
      Nomo de la deksepa litero de la esperanta alfabeto
      <ekz>
        nomoj de la literoj: <klr>[...]</klr>, ko, lo, <tld/>, no
        <klr>[...]</klr>
        <fnt>
          <bib>F</bib>
          <lok>&FE; 1</lok>
        </fnt>,
      </ekz> uzata i.a. kiel:
    </dif>
    <subsnc>
      <uzo tip="fak">MAT</uzo>
      <dif>
        (maj.) romana cifero: <ekz>M (1000);</ekz>
      </dif>
      <trd lng="fr">1000<klr> (chiffre romain)</klr></trd>
      <trd lng="pl">1000<klr> (cyfra rzymska)</klr></trd>
    </subsnc>
    <subsnc>
      <uzo tip="fak">FIZ</uzo>
      <dif>
        (maj.) simbolo de mega: <ekz>M;</ekz>
      </dif>
      <trd lng="fr">m&eacute;ga</trd>
      <trd lng="pl">mega</trd>
    </subsnc>
    <subsnc>
      <uzo tip="fak">FIZ</uzo>
      <dif>
        (min.) simbolo de metro: <ekz>m;</ekz>
      </dif>
      <trd lng="fr">m&egrave;tre</trd>
      <trd lng="pl">metr</trd>
    </subsnc>
    <subsnc>
      <uzo tip="fak">FIZ</uzo>
      <dif>
        (min.) simbolo de mili: <ekz>mm (milimetro).</ekz>
      </dif>
      <trd lng="fr">mili</trd>
      <trd lng="pl">mili</trd>
    </subsnc>
    <trd lng="fr">m<klr> (lettre)</klr></trd>
    <trd lng="pl">m <klr> (17 litera alfabetu Esperanto)</klr></trd>
  </snc>
    <trd lng="ja">&#x30E2;&#x30FC;</trd>
</drv>

</art>

