<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kampanj</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kampan.0o">
  <kap><tld/>o</kap>
  <snc mrk="kampan.0o.MER">
    <dif>
      La&ubreve;plana agado por difinita celo:
      <ekz>
        entrepreni energian <tld/>on por la popolklerigado.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_p;&c_a;&c_n;&c_ib;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Feldzug</trd>,
    <trd>Kampagne</trd>
  </trdgrp>
  <trd lng="en">campaign</trd>
  <trd lng="es">campa&ntilde;a</trd>
  <trd lng="fr">campagne<klr tip="ind">
  (action syst&eacute;matique)</klr></trd>
  <trd lng="hu">kamp&aacute;ny</trd>
  <trd lng="nl">campagne</trd>
  <trd lng="pl">kampania</trd>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_p;&c_a;&c_n;&c_i;&c_ja;</trd>
</drv>

<drv mrk="kampan.0i">
  <kap><tld/>i</kap>
  <snc mrk="kampan.0i.MER">
    <dif>
      Aran&gcirc;i kaj fari <tld/>on:
      <ekz>
        <em>La Loko</em> <tld/>as por ke la Esperanto-teamo partoprenu 
        en la FIFA Monda Pokalo en SudAfriko en 2010
        <fnt>
          <aut>O. Olsson, D. Salomon</aut>,
          <vrk><url ref="http://laloko.org/">
          la loko</url>, esperanta futbala teamo</vrk>,
          <lok>2007-04</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>faire une <ind>campagne</ind></trd>,
    <trd>promouvoir</trd>
  </trdgrp>
  <trd lng="pl">prowadzi&cacute; <ind>kampani&eogonek;</ind></trd>
</drv>
</art>
