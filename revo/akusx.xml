<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: akusx.xml,v 1.46 2022/08/16 05:28:44 revo Exp $">
<kap>
  <ofc>*</ofc><rad>aku&scirc;</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="akusx.0i">
  <kap><ofc>*</ofc><tld/>i <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="&AdE;/fundamento/universala_vortaro.html#av-106">aku&scirc;'</url></lok></fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="akusx.0i.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Elpu&scirc;i homan idon el la utero:
      <ekz>
        virino, kiam &scirc;i <tld/>as, havas mal&gcirc;ojon, <klr>[&#8230;]</klr> sed kiam &scirc;i naskis la
        infaneton, &scirc;i jam ne memoras sian doloregon
        <fnt>
          <bib>NT</bib>
          <lok>Johano 16:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        Klotildo <tld/>is, &scirc;i naskis du &gcirc;emelojn
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekdua</lok>
        </fnt>;
      </ekz>
      <ekz>
        katino <klr>[&#8230;]</klr> <tld/>is tri katidojn
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 6 (68)</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="nask.0i.BIO">naski</ref>
    <refgrp tip="vid">
      <ref cel="abort.0i">aborti</ref>,
      <ref cel="gener.0i.BIO">generi<sncref/></ref>
    </refgrp>
    <rim>
      &leftquot;Aku&scirc;i&rightquot; akcentas la penan, doloran procezon, dum &leftquot;naski&rightquot; akcentas &gcirc;ian rezulton
      (vd la biblian cita&jcirc;on).
    </rim>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_r;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_p;&c_a;&c_r;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="br">gwilioudi&ntilde;<klr tip="ind"> (v.g.)</klr>
    </trd>
    <trdgrp lng="de">
      <trd>niederkommen</trd>,
      <trd>entbinden</trd>,
      <trd>krei&szlig;en</trd>,
      <trd>geb&auml;ren</trd>
    </trdgrp>
    <trdgrp lng="en">
       <trd>be in labor</trd>
       <trd>be in labour</trd>
    </trdgrp>
    <trd lng="es">parir</trd>
    <trdgrp lng="fa">
      <trd>&a_z;&a_A;&f_y;&f_y;&a_d;&a_n;</trd>,
      <trd>&a_w;&a_D;&a_ayn;&zwnj;&a_H;&a_m;&a_l; &f_k;&a_r;&a_d;&a_n;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>accoucher<klr tip="ind"> (d'un enfant)</klr></trd>,
      <trd>mettre au <ind>monde</ind>
      </trd>
    </trdgrp>
    <trd lng="io">akushar</trd>
    <trd lng="nl">bevallen</trd>
    <trdgrp lng="pt">
      <trd>parir</trd>,
      <trd>dar &agrave; luz</trd>,
      <trd>ter <klr>filho</klr></trd>
    </trdgrp>
    <trd lng="sk">porodi&tcaron;</trd>
  </snc>
  <snc mrk="akusx.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malfacile produkti verkon:
      <ekz>
        li <tld/>is anta&ubreve; nelonge dikan tezon.
      </ekz>
      <ekz>
        Nero sciis trovi ta&ubreve;gajn esprimojn eble tial, ke li mem en doloro <tld/>as &ccirc;iun
        verson
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XL</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kov.el0i.FIG">elkovi</ref>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_r;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_p;&c_a;&c_r;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>hervorbringen</trd>,
      <trd><klr tip="amb">sich</klr> abringen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>accoucher<klr tip="ind"> (d'une id&eacute;e)</klr></trd>,
      <trd>pondre<klr tip="ind"> (une id&eacute;e)</klr>
      </trd>
    </trdgrp>
    <trd lng="nl">ter <ind>wereld</ind> brengen</trd>
  </snc>
  <trdgrp lng="de">
    <trd>zur <ind>Welt</ind> bringen</trd>
  </trdgrp>
  <trd lng="en">labour</trd>
  <trd lng="es">dar a luz</trd>
  <trd lng="he">&lamed;&lamed;&dalet;&tav;</trd>
  <trd lng="it">partorire</trd>
  <trd lng="ru">&c_r;&c_o;&c_zh;&c_a;&c_t;&c_mol;</trd>
  <trd lng="uk">&c_n;&c_a;&c_r;&c_o;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="akusx.0o">
  <kap><tld/>o</kap>
  <snc mrk="akusx.0o.FIZL">
    <dif>
      Ago, procezo <tld/>i:
      <ekz>
        venis la tagoj por &scirc;ia <tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>Luko 2:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi iris helpi ian amikinon en <tld/>o
        <fnt>
          <bib>GDn</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i plendas la dolorojn de <tld/>o
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XL</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o certe ne estos facila, &ccirc;ar la infano estas malbone lokita
        <fnt>
          <bib>KrB</bib>
          <lok>Por paro amanta &ccirc;iu loketo sufi&ccirc;as</lok>
        </fnt>;
      </ekz>
      <ekz>
        provokita <tld/>o 
        <fnt><vrk><url ref="http://vortareto.free.fr/dictionnaire/fr-esp/a.htm">
        Medicina vortareto franca-esperanta</url></vrk>,</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Entbindung</trd>,
    <trd>Niederkunft</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>labor <klr>(childbirth)</klr></trd>,
    <trd>labour <klr>(childbirth)</klr></trd>,
    <trd>delivery <klr>(childbirth)</klr></trd>
  </trdgrp>
  <trd lng="es">parto</trd>
  <trd lng="fr">accouchement</trd>
  <trd lng="io">akusho</trd>
  <trdgrp lng="nl">
    <trd>bevalling</trd>,
    <trd>bevallen<klr tip="ind"> (zn.)</klr></trd>
  </trdgrp>
</drv>
  
<drv mrk="akusx.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="akusx.0igi.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Helpi la <tld/>on:
      <ekz>
        kiam vi <tld/>igos la Hebreinojn, rigardu la ku&scirc;ejon, se estas filo, mortigu lin
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 1:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        kamparano venigis novan kuraciston, por <tld/>igi sian edzinon
        <fnt>
          <bib>KrB</bib>
          <lok>Dissaltos la obstinaj baroj</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>igis multe da &scirc;afinoj, sed mi memoras nur tiun
        <fnt>
          <bib>Monato</bib>
          <aut>Christian Declerck</aut>
          <vrk><url ref="https://www.monato.be/2005/008772.php"
              >Atestas viktimo de honorkrimo</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_m;&c_a;&c_c;&c_mol; &c_r;&c_o;&c_d;&c_y;</trd>,
    <trd>&c_b;&c_a;&c_b;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">gwilioudi&ntilde;<klr tip="ind"> (v.k.)</klr></trd>
  <trdgrp lng="de">
    <trd>Geburtshilfe <ind>leisten</ind></trd>,
    <trd>Hebammendienste <ind>leisten</ind></trd>
  </trdgrp>
  <trd lng="en">induce labor</trd>
  <trd lng="es">asistir, ayudar a una partera</trd>
  <trd lng="fa">&a_z;&a_A;&f_y;&a_A;&a_n;&a_d;&a_n;</trd>
  <trd lng="fr">accoucher<klr tip="ind"> (une femme)</klr></trd>
  <trd lng="he">&lamed;&yod;&yod;&lamed;&dalet;</trd>
  <trdgrp lng="nl">
    <trd>verlossen<klr tip="ind"> (bij de bevalling helpen</klr></trd>,
    <trd>bij de <ind>bevalling</ind> helpen</trd>
  </trdgrp>
  <trd lng="pt">partejar</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;
  <ind>&c_r;&c_o;&c_d;&c_y;</ind></trd>
</drv>

<drv mrk="akusx.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="akusx.0ilo.MED">
    <uzo tip="fak">MED</uzo>
    <ref tip="dif" cel="forcep.0o">forcepso</ref>
  </snc>
  <trd lng="br">kregeier<klr tip="ind"> (gwilioudi&ntilde;)</klr></trd>
  <trd lng="de">Geburtszange</trd>
  <trd lng="en">forceps</trd>
  <trd lng="es">f&oacute;rceps</trd>
  <trd lng="fa">&a_f;&a_r;&a_s;&f_p;&a_s;</trd>
  <trd lng="fr">forceps</trd>
  <trd lng="it">forcipe</trd>
  <trdgrp lng="nl">
    <trd>verlostang</trd>,
    <trd>forceps</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_k;&c_u;&c_sh;&c_je;&c_r;&c_s;&c_k;&c_i;&c_je;&c_shch;&c_i;&c_p;&c_c;&c_y;</trd>
</drv>

<drv mrk="akusx.0isto">
  <kap><tld/>isto, <var><kap><tld/>igisto</kap></var></kap>
  <snc mrk="akusx.0isto.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Homo, kies profesio estas zorgi la <tld/>ojn:
      <ekz>
        la <tld/>igisto subite eniras la kuirejon kaj diras: &ndash; nur per &ccirc;izilo kaj
        martelo mi sukcesos
        <fnt>
          <bib>KrB</bib>
          <lok>Dissaltos la obstinaj baroj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_u;&c_sh;&c_e;&c_r;&c_k;&c_a;</trd>
  <trd lng="br">amiegez</trd>
  <trd lng="de">Hebamme</trd>
  <trdgrp lng="en">
    <trd>midwife</trd>,
    <trd>accoucheuse</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>partera</trd>,
    <trd>comadrona</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_m;&a_A;&a_m;&a_A;</trd>,
    <trd>&a_q;&a_A;&a_b;&a_l;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">sage-femme</trd>
  <trd lng="he">&mem;&#1456;&yod;&#1463;&lamed;&#1462;&dalet;&#1462;&tav;</trd>
  <trd lng="io">akushisto</trd>
  <trdgrp lng="it">
    <trd>ostetrica</trd>,
    <trd>levatrice</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verloskundige</trd>,
    <trd>vroedvrouw</trd>,
    <trd>vroedkundige</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>parteira</trd>,
    <trd>obstetriz</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_k;&c_u;&c_sh;&c_je;&c_r;&c_k;&c_a;</trd>
</drv>

<drv mrk="akusx.0istino">
  <kap><ofc>*</ofc> <tld/>istino <fnt><bib>F</bib>, <lok>Universala
  Vortaro, <url ref="&AdE;/fundamento/universala_vortaro.html#av-4968">aku&scirc;'ist'in'</url></lok></fnt>,
  <var><kap><tld/>igistino</kap></var></kap>
  <snc mrk="akusx.0istino.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Virino, kies profesio estas zorgi la <tld/>ojn:
      <ekz>
        la <tld/>istino prenis kaj alligis al la mano ru&gcirc;an fadenon, dirante: &ccirc;i tiu
        eliris la unua
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 38:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam la <tld/>istino lin alportis al mi, mi levis lin al la &ccirc;ielo kaj ekkriis
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_u;&c_sh;&c_e;&c_r;&c_k;&c_a;</trd>
  <trd lng="br">amiegez</trd>
  <trd lng="de">Hebamme</trd>
  <trdgrp lng="en">
    <trd>midwife</trd>,
    <trd>accoucheuse</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>partera</trd>,
    <trd>comadrona</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_m;&a_A;&a_m;&a_A;</trd>,
    <trd>&a_q;&a_A;&a_b;&a_l;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">sage-femme</trd>
  <trd lng="he">&mem;&#1456;&yod;&#1463;&lamed;&#1462;&dalet;&#1462;&tav;</trd>
  <trd lng="io">akushistino</trd>
  <trdgrp lng="it">
    <trd>ostetrica</trd>,
    <trd>levatrice</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verloskundige</trd>,
    <trd>vroedvrouw</trd>,
    <trd>vroedkundige</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>parteira</trd>,
    <trd>obstetriz</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_k;&c_u;&c_sh;&c_je;&c_r;&c_k;&c_a;</trd>
</drv>

<drv mrk="akusx.0intino">
  <kap><tld/>into, <var><kap><tld/>intino</kap></var></kap>
  <snc mrk="akusx.0intino.MED">
    <dif>
      &Jcirc;us <tld/>inta patrino.
    </dif>
    <ref tip="vid" cel="nask.0antino">naskantino</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_d;&c_z;&c_ib;&c_h;&c_a;</trd>
  <trd lng="de">junge <ind>Mutter</ind></trd>
  <trd lng="en">new mother</trd>
  <trd lng="fa">&a_z;&a_A;&a_y_hamza;&a_w;</trd>
  <trd lng="fr">accouch&eacute;e</trd>
  <trd lng="he">&mem;&#1456;&yod;&#1463;&lamed;&#1462;&dalet;&#1462;&tav;</trd>
  <trd lng="it">puerpera</trd>
  <trd lng="nl">jonge <ind>moeder</ind></trd>
  <trd lng="pt">parturiente</trd>
  <trd lng="ru">&c_r;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_c;&c_a;<klr>
  (&c_r;&c_o;&c_d;&c_i;&c_v;&c_sh;&c_a;&c_ja;)</klr></trd>
</drv>

<drv mrk="akusx.0antino">
  <kap><tld/>anto, <var><kap><tld/>antino</kap></var></kap>
  <snc mrk="akusx.0antino.MED">
    <dif>
      <tld lit="A"/>anta persono:
      <ekz>
        teruro ilin atakis, tremo, kiel &ccirc;e <tld/>antino
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 48:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi preferus mem esti <tld/>antino, ol flegi aliajn
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Krei&szlig;ende</trd>,
    <trd>Geb&auml;rende</trd>
  </trdgrp>
  <trd lng="fa">&a_z;&a_A;&a_y_hamza;&a_w;</trd>
  <trd lng="fr">parturiente</trd>
  <trd lng="he">&mem;&#1456;&yod;&#1463;&lamed;&#1462;&dalet;&#1462;&tav;</trd>
  <trd lng="it">partoriente</trd>
  <trd lng="nl">kraamvrouw</trd>
</drv>

<drv mrk="akusx.0arto">
  <kap><tld/>arto</kap>
  <snc mrk="akusx.0arto.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Tekniko de la <ref tip="vid" cel="akusx.0isto"><tld/>isto</ref>.
    </dif>
    <trdgrp lng="fa">
      <trd>&a_m;&a_A;&a_m;&a_A;&f_y;&f_y;</trd>,
      <trd>&a_q;&a_A;&a_b;&a_l;&f_g;&f_y;</trd>
    </trdgrp>
    <trd lng="fr">obst&eacute;trique</trd>
    <trd lng="nl">kraamleer</trd>
  </snc>
  <snc mrk="akusx.0arto.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Metodo de la sokratismo por zorgi la akiron de konoj kiel la
      <tld/>isto zorgas la aku&scirc;ojn.
    </dif>
  </snc>
  <trd lng="de">Geburtshilfe</trd>
  <trd lng="fr">ma&iuml;eutique</trd>
  <trd lng="it">maieutica</trd>
</drv>

<drv mrk="akusx.0doloro">
  <kap><tld/>doloro</kap>
  <snc>
    <dif>
      Doloro ka&ubreve;zita de spasmo de la ventro, kiu prepari&gcirc;as por la
      <tld/>o.
    </dif>
<ref tip="vid" cel="nask.0o">naskodoloro</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Wehen</trd>,
    <trd>Geburtsschmerz</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>labor pain</trd>,
    <trd>labour pain</trd>
  </trdgrp>
  <trd lng="fa">&a_d;&a_r;&a_d; <ind>&a_z;&a_A;&f_y;&a_m;&a_A;&a_n;</ind></trd>
  <trd lng="fr">douleurs<klr> (d'accouchement)</klr></trd>
  <trd lng="it">doglia</trd>
  <trd lng="nl">wee&euml;n</trd>
</drv>

<drv mrk="akusx.0peno">
  <kap><tld/>peno</kap>
  <snc>
    <dif>
      Spasmoj de la ventro anta&ubreve; la <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Wehen</trd>
  <trdgrp lng="en">
    <trd>contraction <klr>(childbirth)</klr></trd>
  </trdgrp>
  <trd lng="es">contracci&oacute;n<klr tip="amb"> (de parto)</klr></trd>
  <trd lng="fr">travail<klr> (d'accouchement)</klr></trd>
  <trd lng="it">travaglio</trd>
</drv>
</art>
