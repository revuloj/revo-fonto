</kap>
<drv mrk="akusx.0i">
  <kap><ofc>*</ofc><tld/>i <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="&AdE;/fundamento/universala_vortaro.html#av-106">aku&scirc;'</url></lok></fnt></kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="akusx.0i.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Elpu&scirc;i idon el la utero:
      <ekz>
        virino, kiam &scirc;i <tld/>as, havas mal&gcirc;ojon, <klr>[&#8230;]</klr> sed kiam &scirc;i naskis la
        infaneton, &scirc;i jam ne memoras sian doloregon
        <fnt>
          <bib>NT</bib>
          <lok>&Joh; 16:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        Klotildo <tld/>is, &scirc;i naskis du &gcirc;emelojn
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro dekdua</lok>
        </fnt>;
      </ekz>
      <ekz>
        katino <klr>[&#8230;]</klr> <tld/>is tri katidojn
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 6 (68)</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="nask.0i.BIO">naski</ref>
    <refgrp tip="vid">
      <ref cel="abort.0i">aborti</ref>,
      <ref cel="gener.0i.BIO">generi<sncref/></ref>
    </refgrp>
    <rim>
      &leftquot;Aku&scirc;i&rightquot; akcentas la penan, doloran procezon, dum &leftquot;naski&rightquot; akcentas &gcirc;ian rezulton
      (vd la biblian cita&jcirc;on).
    </rim>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_r;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_p;&c_a;&c_r;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="br">gwilioudi&ntilde;<klr tip="ind"> (v.g.)</klr>
    </trd>
    <trdgrp lng="de">
      <trd>niederkommen</trd>,
      <trd>entbinden</trd>,
      <trd>krei&szlig;en</trd>,
      <trd>geb&auml;ren</trd>
    </trdgrp>
    <trdgrp lng="en">
       <trd>be in labor</trd>
       <trd>be in labour</trd>
    </trdgrp>
    <trd lng="es">parir</trd>
    <trdgrp lng="fa">
      <trd>&a_z;&a_A;&f_y;&f_y;&a_d;&a_n;</trd>,
      <trd>&a_w;&a_D;&a_ayn;&#x200C;&a_H;&a_m;&a_l; &f_k;&a_r;&a_d;&a_n;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>accoucher<klr tip="ind"> (d'un enfant)</klr></trd>,
      <trd>mettre au <ind>monde</ind>
      </trd>
    </trdgrp>
    <trd lng="io">akushar</trd>
    <trd lng="nl">bevallen</trd>
    <trdgrp lng="pt">
      <trd>parir</trd>,
      <trd>dar &agrave; luz</trd>,
      <trd>ter <klr>filho</klr></trd>
    </trdgrp>
    <trd lng="sk">porodi&tcaron;</trd>
    <trdgrp lng="zh">
    <trd>&#x4F7F;&#x751F;&#x5C0F;&#x725B; <pr>sh&#x1D0;sh&emacron;ngxi&#x1CE;oni&#xFA;</pr></trd>,
    <trd>&#x4F7F;&#x751F;&#x5C0F;&#x7F8A; <pr>sh&#x1D0;sh&emacron;ngxi&#x1CE;oy&#xE1;ng</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="akusx.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malfacile produkti verkon:
      <ekz>
        li <tld/>is anta&ubreve; nelonge dikan tezon;
      </ekz>
      <ekz>
        Nero sciis trovi ta&ubreve;gajn esprimojn eble tial, ke li mem en doloro <tld/>as &ccirc;iun
        verson
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro 40a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kov.el0i.FIG">elkovi</ref>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_r;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_p;&c_a;&c_r;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>hervorbringen</trd>,
      <trd><klr tip="amb">sich</klr> abringen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>accoucher<klr tip="ind"> (d'une id&eacute;e)</klr></trd>,
      <trd>pondre<klr tip="ind"> (une id&eacute;e)</klr>
      </trd>
    </trdgrp>
    <trd lng="nl">ter <ind>wereld</ind> brengen</trd>
    <trdgrp lng="zh">
    <trd>&#x521B;&#x65B0; <pr>chu&#xE0;ngx&#x12B;n</pr></trd>,
    <trd>&#21109;&#x65B0; <pr>chu&#xE0;ngx&#x12B;n</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="de">
    <trd>zur <ind>Welt</ind> bringen</trd>
  </trdgrp>
  <trd lng="en">labour</trd>
  <trd lng="es">dar a luz</trd>
  <trd lng="he">&lamed;&lamed;&dalet;&tav;</trd>
  <trd lng="it">partorire</trd>
  <trdgrp lng="ja">
    <trd>&#x51FA;&#29987;&#12377;&#x308B;  <pr>&#12375;&#12421;&#12387;&#12373;&#12435;&#12377;&#x308B;</pr></trd>,
    <trd>&#20998;&#x5A29;&#12377;&#x308B;  <pr>&#12406;&#12435;&#12409;&#12435;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_o;&c_zh;&c_a;&c_t;&c_mol;</trd>
  <trd lng="uk">&c_n;&c_a;&c_r;&c_o;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="akusx.0o">
  <kap><tld/>o</kap>
  <snc mrk="akusx.0o.FIZL">
    <dif>
      Ago, procezo <tld/>i:
      <ekz>
        venis la tagoj por &scirc;ia <tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>&Luk; 2:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi iris helpi ian amikinon en <tld/>o
        <fnt>
          <bib>GDn</bib>
          <lok>Akto tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i plendas la dolorojn de <tld/>o
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro 40a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o certe ne estos facila, &ccirc;ar la infano estas malbone lokita
        <fnt>
          <bib>KrB</bib>
          <lok>Por paro amanta &ccirc;iu loketo sufi&ccirc;as</lok>
        </fnt>;
      </ekz>
      <ekz>
        provokita <tld/>o
        <fnt><vrk><url ref="http://vortareto.free.fr/dictionnaire/fr-esp/a.htm">
        Medicina vortareto franca-esperanta</url></vrk>,</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_r;&c_a;&c_d;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="de">
    <trd>Entbindung</trd>,
    <trd>Niederkunft</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>labor <klr>(childbirth)</klr></trd>,
    <trd>labour <klr>(childbirth)</klr></trd>,
    <trd>delivery <klr>(childbirth)</klr></trd>
  </trdgrp>
  <trd lng="es">parto</trd>
  <trd lng="fr">accouchement</trd>
  <trd lng="io">akusho</trd>
  <trdgrp lng="ja">
    <trd>&#x51FA;&#29987;  <pr>&#12375;&#12421;&#12387;&#12373;&#12435;</pr></trd>,
    <trd>&#20998;&#x5A29;  <pr>&#12406;&#12435;&#12409;&#12435;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bevalling</trd>,
    <trd>bevallen<klr tip="ind"> (zn.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="akusx.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="akusx.0ejo.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="kurac.0ejo.MED">Kuracejo</ref> aran&gcirc;ita kaj ekipita por <tld/>oj:
      <ekz>
        konstrui fidindajn &scirc;irmejojn, tendojn por &ccirc;iu familio, hospitalojn kaj <tld/>ejojn
        <fnt>
          <aut>F. Monshet Nhy Wa Dana</aut>
          <vrk><url
          ref="http://verkoj.com/verkistoj/wa-dana-monshet-nhy/grandaj-malbonoj-grandaj-rimedoj/">
          Grandaj Malbonoj, Malgrandaj Rimedoj</url></vrk>
          <lok>verkoj.com, [vidita en 2024]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">maternit&eacute;</trd>
</drv>

<drv mrk="akusx.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="akusx.0igi.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Helpi la <tld/>on:
      <ekz>
        kiam vi <tld/>igos la Hebreinojn, rigardu la ku&scirc;ejon, se estas filo, mortigu lin
        <fnt>
          <bib>MT</bib>
          <lok>&Eli; 1:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        kamparano venigis novan kuraciston, por <tld/>igi sian edzinon
        <fnt>
          <bib>KrB</bib>
          <lok>Dissaltos la obstinaj baroj</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>igis multe da &scirc;afinoj, sed mi memoras nur tiun
        <fnt>
          <bib>Monato</bib>
          <aut>Christian Declerck</aut>
          <vrk><url ref="&Monato;/2005/008772.php">Atestas viktimo de honorkrimo</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_m;&c_a;&c_c;&c_mol; &c_r;&c_o;&c_d;&c_y;</trd>,
    <trd>&c_b;&c_a;&c_b;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">gwilioudi&ntilde;<klr tip="ind"> (v.k.)</klr></trd>
  <trdgrp lng="de">
    <trd>Geburtshilfe <ind>leisten</ind></trd>,
    <trd>Hebammendienste <ind>leisten</ind></trd>
  </trdgrp>
  <trd lng="en">induce labor</trd>
  <trd lng="es">asistir, ayudar a una partera</trd>
  <trd lng="fa">&a_z;&a_A;&f_y;&a_A;&a_n;&a_d;&a_n;</trd>
  <trd lng="fr">accoucher<klr tip="ind"> (une femme)</klr></trd>
  <trd lng="he">&lamed;&yod;&yod;&lamed;&dalet;</trd>
  <trd lng="ja">&#x51FA;&#29987;&#12373;&#x305B;&#x308B;  <pr>&#12375;&#12421;&#12387;&#12373;&#12435;&#12373;&#x305B;&#x308B;</pr></trd>
  <trdgrp lng="nl">
    <trd>verlossen<klr tip="ind"> (bij de bevalling helpen</klr></trd>,
    <trd>bij de <ind>bevalling</ind> helpen</trd>
  </trdgrp>
  <trd lng="pt">partejar</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;
  <ind>&c_r;&c_o;&c_d;&c_y;</ind></trd>
</drv>

