<drv mrk="akusx.0isto">
  <kap><tld/>isto, <var><kap><tld/>igisto</kap></var></kap>
  <snc mrk="akusx.0isto.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Homo, kies profesio estas zorgi la <tld/>ojn:
      <ekz>
        la <tld/>igisto subite eniras la kuirejon kaj diras: &ndash; nur per &ccirc;izilo kaj
        martelo mi sukcesos
        <fnt>
          <bib>KrB</bib>
          <lok>Dissaltos la obstinaj baroj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_u;&c_sh;&c_e;&c_r;</trd>
  <trd lng="br">amiegez</trd>
  <trd lng="de">Hebamme</trd>
  <trdgrp lng="en">
    <trd>midwife</trd>,
    <trd>accoucheuse</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>partera</trd>,
    <trd>comadrona</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_m;&a_A;&a_m;&a_A;</trd>,
    <trd>&a_q;&a_A;&a_b;&a_l;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">sage-femme</trd>
  <trd lng="he">&mem;&#x5B0;&yod;&#x5B7;&lamed;&#x5B6;&dalet;&#x5B6;&tav;</trd>
  <trd lng="io">akushisto</trd>
  <trdgrp lng="it">
    <trd>ostetrica</trd>,
    <trd>levatrice</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verloskundige</trd>,
    <trd>vroedvrouw</trd>,
    <trd>vroedkundige</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>parteira</trd>,
    <trd>obstetriz</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_k;&c_u;&c_sh;&c_je;&c_r;&c_k;&c_a;</trd>
</drv>

<drv mrk="akusx.0istino">
  <kap><ofc>*</ofc> <tld/>istino <fnt><bib>F</bib>, <lok>Universala
  Vortaro, <url
  ref="&AdE;/fundamento/universala_vortaro.html#av-4968">aku&scirc;'ist'in'</url></lok></fnt>,
  <var><kap><tld/>igistino</kap></var></kap>
  <snc mrk="akusx.0istino.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Virino, kies profesio estas zorgi la <tld/>ojn:
      <ekz>
        la <tld/>istino prenis kaj alligis al la mano ru&gcirc;an fadenon, dirante: &ccirc;i tiu
        eliris la unua
        <fnt>
          <bib>MT</bib>
          <lok>&Gen; 38:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam la <tld/>istino lin alportis al mi, mi levis lin al la &ccirc;ielo kaj ekkriis
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto unua</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x52A9;&#x4EA7;&#x58EB; <pr>zh&#xF9;ch&#x1CE;nsh&#xEC;</pr></trd>,
    <trd>&#x52A9;&#29986;&#x58EB; <pr>zh&#xF9;ch&#x1CE;nsh&#xEC;</pr></trd>,
    <trd>&#x63A5;&#x751F;&#x5A46; <pr>ji&emacron;sh&emacron;ngp&#xF3;</pr></trd>,
    <trd>&#x4EA7;&#x5A46; <pr>ch&#x1CE;np&#xF3;</pr></trd>,
    <trd>&#29986;&#x5A46; <pr>ch&#x1CE;np&#xF3;</pr></trd>,
    <trd>&#x63A5;&#x751F;&#21592; <pr>ji&emacron;sh&emacron;ngyu&#xE1;n</pr></trd>,
    <trd>&#x63A5;&#x751F;&#x54E1; <pr>ji&emacron;sh&emacron;ngyu&#xE1;n</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_k;&c_u;&c_sh;&c_e;&c_r;&c_k;&c_a;</trd>
  <trd lng="br">amiegez</trd>
  <trd lng="de">Hebamme</trd>
  <trdgrp lng="en">
    <trd>midwife</trd>,
    <trd>accoucheuse</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>partera</trd>,
    <trd>comadrona</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_m;&a_A;&a_m;&a_A;</trd>,
    <trd>&a_q;&a_A;&a_b;&a_l;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">sage-femme</trd>
  <trd lng="he">&mem;&#x5B0;&yod;&#x5B7;&lamed;&#x5B6;&dalet;&#x5B6;&tav;</trd>
  <trd lng="io">akushistino</trd>
  <trdgrp lng="it">
    <trd>ostetrica</trd>,
    <trd>levatrice</trd>
  </trdgrp>
  <trd lng="ja">&#x52A9;&#29987;&#x5A66;  <pr>&#12376;&#12423;&#12373;&#12435;&#12407;</pr></trd>
  <trdgrp lng="nl">
    <trd>verloskundige</trd>,
    <trd>vroedvrouw</trd>,
    <trd>vroedkundige</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>parteira</trd>,
    <trd>obstetriz</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_k;&c_u;&c_sh;&c_je;&c_r;&c_k;&c_a;</trd>
</drv>

