<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: akusx.xml,v 1.40 2021/02/28 15:17:51 revo Exp $">
<kap>
  <ofc>*</ofc><rad>aku&scirc;</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="akusx.0i">
  <kap><ofc>*</ofc><tld/>i <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-106">aku&scirc;'</url></lok></fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="akusx.0i.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Elpu&scirc;i homan idon el la utero:
      <ekz>
        la patrino <tld/>is belan infanon
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_r;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_p;&c_a;&c_r;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="br">gwilioudi&ntilde;<klr tip="ind"> (v.g.)</klr>
    </trd>
    <trdgrp lng="de">
      <trd>geb&auml;ren</trd>,
      <trd>krei&szlig;en</trd>,
      <trd>niederkommen</trd>
    </trdgrp>
    <trd lng="en">be in labor</trd>
    <trd lng="es">parir</trd>
    <trdgrp lng="fa">
      <trd>&a_z;&a_A;&f_y;&f_y;&a_d;&a_n;</trd>,
      <trd>&a_w;&a_D;&a_ayn;&zwnj;&a_H;&a_m;&a_l; &f_k;&a_r;&a_d;&a_n;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>accoucher<klr tip="ind"> (d'un enfant)</klr></trd>,
      <trd>mettre au <ind>monde</ind>
      </trd>
    </trdgrp>
    <trd lng="io">akushar</trd>
    <trd lng="nl">bevallen</trd>
    <trdgrp lng="pt">
      <trd>parir</trd>,
      <trd>dar &agrave; luz</trd>,
      <trd>ter <klr>filho</klr></trd>
    </trdgrp>
    <trd lng="sk">porodi&tcaron;</trd>
  </snc>
  <snc mrk="akusx.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malfacile produkti verkon:
      <ekz>
        li <tld/>is anta&ubreve; nelonge dikan tezon.
      </ekz>
    </dif>
    <ref tip="vid" cel="kov.el0i.FIG">elkovi</ref>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_r;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_p;&c_a;&c_r;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>hervorbringen</trd>,
      <trd><klr tip="amb">sich</klr> abringen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>accoucher<klr tip="ind"> (d'une id&eacute;e)</klr></trd>,
      <trd>pondre<klr tip="ind"> (une id&eacute;e)</klr>
      </trd>
    </trdgrp>
    <trd lng="nl">ter <ind>wereld</ind> brengen</trd>
  </snc>
  <trdgrp lng="de">
    <trd>zur <ind>Welt</ind> bringen</trd>
  </trdgrp>
  <trd lng="es">dar a luz</trd>
  <trd lng="he">&#x5DC;&#x5DC;&#x5D3;&#x5EA;</trd>
  <trd lng="it">partorire</trd>
  <trd lng="ru">&c_r;&c_o;&c_zh;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="akusx.0o">
  <kap><tld/>o</kap>
  <snc mrk="akusx.0o.FIZL">
    <dif>
      Ago <tld/>i, rezulto de la ago:
      <ekz>
        provokita <tld/>o 
        <fnt><vrk><url ref="http://vortareto.free.fr/dictionnaire/fr-esp/a.htm">
        Medicina vortareto franca-esperanta</url></vrk>,</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">parto</trd>
  <trd lng="fr">accouchement</trd>
  <trd lng="io">akusho</trd>
  <trdgrp lng="nl">
    <trd>bevalling</trd>,
    <trd>bevallen<klr tip="ind"> (zn.)</klr></trd>
  </trdgrp>
</drv>
  
<drv mrk="akusx.0igi">
  <kap><tld/>igi<fnt>Z</fnt></kap>
  <snc mrk="akusx.0igi.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Helpi la <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_m;&c_a;&c_c;&c_mol; &c_r;&c_o;&c_d;&c_y;</trd>,
    <trd>&c_b;&c_a;&c_b;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">gwilioudi&ntilde;<klr tip="ind"> (v.k.)</klr></trd>
  <trdgrp lng="de">
    <trd>Geburtshilfe <ind>leisten</ind></trd>,
    <trd>Hebammendienste <ind>leisten</ind></trd>
  </trdgrp>
  <trd lng="en">induce labor</trd>
  <trd lng="es">asistir, ayudar a una partera</trd>
  <trd lng="fa">&a_z;&a_A;&f_y;&a_A;&a_n;&a_d;&a_n;</trd>
  <trd lng="fr">accoucher<klr tip="ind"> (une femme)</klr></trd>
  <trd lng="he">&#x5DC;&#x5D9;&#x5D9;&#x5DC;&#x5D3;</trd>
  <trdgrp lng="nl">
    <trd>verlossen<klr tip="ind"> (bij de bevalling helpen</klr></trd>,
    <trd>bij de <ind>bevalling</ind> helpen</trd>
  </trdgrp>
  <trd lng="pt">partejar</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol; <ind>&c_r;&c_o;&c_d;&c_y;</ind></trd>
</drv>

<drv mrk="akusx.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="akusx.0ilo.MED">
    <uzo tip="fak">MED</uzo>
    <ref tip="dif" cel="forcep.0o">forcepso</ref>
  </snc>
  <trd lng="br">kregeier<klr tip="ind"> (gwilioudi&ntilde;)</klr></trd>
  <trd lng="de">Geburtszange</trd>
  <trd lng="en">forceps</trd>
  <trd lng="es">f&oacute;rceps</trd>
  <trd lng="fa">&a_f;&a_r;&a_s;&f_p;&a_s;</trd>
  <trd lng="fr">forceps</trd>
  <trd lng="it">forcipe</trd>
  <trdgrp lng="nl">
    <trd>verlostang</trd>,
    <trd>forceps</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_k;&c_u;&c_sh;&c_je;&c_r;&c_s;&c_k;&c_i;&c_je;&c_shch;&c_i;&c_p;&c_c;&c_y;</trd>
</drv>

<drv mrk="akusx.0isto">
  <kap><tld/>isto, <var><kap><tld/>igisto</kap></var></kap>
  <snc mrk="akusx.0isto.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Homo, kies profesio estas zorgi la <tld/>ojn.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_u;&c_sh;&c_e;&c_r;&c_k;&c_a;</trd>
  <trd lng="br">amiegez</trd>
  <trd lng="de">Hebamme</trd>
  <trdgrp lng="en">
    <trd>midwife</trd>,
    <trd>accoucheuse</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>partera</trd>,
    <trd>comadrona</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_m;&a_A;&a_m;&a_A;</trd>,
    <trd>&a_q;&a_A;&a_b;&a_l;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">sage-femme</trd>
  <trd lng="he">&#x5DE;&#x5B0;&#x5D9;&#x5B7;&#x5DC;&#x5B6;&#x5D3;&#x5B6;&#x5EA;</trd>
  <trd lng="io">akushisto</trd>
  <trdgrp lng="it">
    <trd>ostetrica</trd>,
    <trd>levatrice</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verloskundige</trd>,
    <trd>vroedvrouw</trd>,
    <trd>vroedkundige</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>parteira</trd>,
    <trd>obstetriz</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_k;&c_u;&c_sh;&c_je;&c_r;&c_k;&c_a;</trd>
</drv>

<drv mrk="akusx.0istino">
  <kap><ofc>*</ofc> <tld/>istino <fnt><bib>F</bib>, <lok>Universala
  Vortaro, <url ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-4968">aku&scirc;'ist'in'</url></lok></fnt>,
  <var><kap><tld/>igistino</kap></var></kap>
  <snc mrk="akusx.0istino.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Virino, kies profesio estas zorgi la <tld/>ojn.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_u;&c_sh;&c_e;&c_r;&c_k;&c_a;</trd>
  <trd lng="br">amiegez</trd>
  <trd lng="de">Hebamme</trd>
  <trdgrp lng="en">
    <trd>midwife</trd>,
    <trd>accoucheuse</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>partera</trd>,
    <trd>comadrona</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_m;&a_A;&a_m;&a_A;</trd>,
    <trd>&a_q;&a_A;&a_b;&a_l;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">sage-femme</trd>
  <trd lng="he">&#x5DE;&#x5B0;&#x5D9;&#x5B7;&#x5DC;&#x5B6;&#x5D3;&#x5B6;&#x5EA;</trd>
  <trd lng="io">akushistino</trd>
  <trdgrp lng="it">
    <trd>ostetrica</trd>,
    <trd>levatrice</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verloskundige</trd>,
    <trd>vroedvrouw</trd>,
    <trd>vroedkundige</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>parteira</trd>,
    <trd>obstetriz</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_k;&c_u;&c_sh;&c_je;&c_r;&c_k;&c_a;</trd>
</drv>

<drv mrk="akusx.0intino">
  <kap><tld/>into, <var><kap><tld/>intino</kap></var></kap>
  <snc mrk="akusx.0intino.MED">
    <dif>
      &Jcirc;us <tld/>inta persono.
    </dif>
    <ref tip="vid" cel="nask.0antino">naskantino</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_d;&c_z;&c_ib;&c_h;&c_a;</trd>
  <trd lng="de">junge <ind>Mutter</ind></trd>
  <trd lng="en">new mother</trd>
  <trd lng="fa">&a_z;&a_A;&a_y_hamza;&a_w;</trd>
  <trd lng="fr">accouch&eacute;e</trd>
  <trd lng="he">&#x5DE;&#x5B0;&#x5D9;&#x5B7;&#x5DC;&#x5B6;&#x5D3;&#x5B6;&#x5EA;</trd>
  <trd lng="it">puerpera</trd>
  <trd lng="nl">jonge <ind>moeder</ind></trd>
  <trd lng="pt">parturiente</trd>
  <trd lng="ru">&c_r;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_c;&c_a;<klr> (&c_r;&c_o;&c_d;&c_i;&c_v;&c_sh;&c_a;&c_ja;)</klr></trd>
</drv>

<drv mrk="akusx.0antino">
  <kap><tld/>anto, <var><kap><tld/>antino</kap></var></kap>
  <snc mrk="akusx.0antino.MED">
    <dif>
      <tld lit="A"/>anta persono.
    </dif>
  </snc>
  <trd lng="fa">&a_z;&a_A;&a_y_hamza;&a_w;</trd>
  <trd lng="fr">parturiente</trd>
  <trd lng="he">&#x5DE;&#x5B0;&#x5D9;&#x5B7;&#x5DC;&#x5B6;&#x5D3;&#x5B6;&#x5EA;</trd>
  <trd lng="it">partoriente</trd>
  <trd lng="nl">kraamvrouw</trd>
</drv>

<drv mrk="akusx.0arto">
  <kap><tld/>arto</kap>
  <snc mrk="akusx.0arto.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Tekniko de la <ref tip="vid" cel="akusx.0isto"><tld/>isto</ref>.
    </dif>
    <trdgrp lng="fa">
      <trd>&a_m;&a_A;&a_m;&a_A;&f_y;&f_y;</trd>,
      <trd>&a_q;&a_A;&a_b;&a_l;&f_g;&f_y;</trd>
    </trdgrp>
    <trd lng="fr">obst&eacute;trique</trd>
    <trd lng="nl">kraamleer</trd>
  </snc>
  <snc mrk="akusx.0arto.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Metodo de la sokratismo por zorgi la akiron de konoj kiel la
      <tld/>isto zorgas la aku&scirc;ojn.
    </dif>
  </snc>
  <trd lng="fr">ma&iuml;eutique</trd>
  <trd lng="it">maieutica</trd>
</drv>

<drv mrk="akusx.0doloro">
  <kap><tld/>doloro</kap>
  <snc>
    <dif>
      Doloro ka&ubreve;zita de spasmo de la ventro, kiu prepari&gcirc;as por la
      <tld/>o.
    </dif>
  </snc>
  <trd lng="fa">&a_d;&a_r;&a_d; <ind>&a_z;&a_A;&f_y;&a_m;&a_A;&a_n;</ind></trd>
  <trd lng="fr">douleurs<klr> (d'accouchement)</klr></trd>
  <trd lng="it">doglia</trd>
  <trd lng="nl">wee&euml;n</trd>
</drv>

<drv mrk="akusx.0peno">
  <kap><tld/>peno</kap>
  <snc>
    <dif>
      Spasmoj de la ventro anta&ubreve; la <tld/>o.
    </dif>
  </snc>
  <trd lng="en">labour</trd>
  <trd lng="es">contracci&oacute;n<klr tip="amb"> (de parto)</klr></trd>
  <trd lng="fr">travail<klr> (d'accouchement)</klr></trd>
  <trd lng="it">travaglio</trd>
</drv>
</art>
