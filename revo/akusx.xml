<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc><rad>aku&scirc;</rad>/i
</kap>
<drv mrk="akusx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Elpu&scirc;i homan idon el la utero:
      <ekz>
        la patrino <tld/>is belan infanon
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_r;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_p;&c_a;&c_r;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="es">parir</trd>
    <trdgrp lng="fr">
      <trd>accoucher <klr>(d'un enfant)</klr></trd>,
      <trd>mettre au <ind>monde</ind></trd>
    </trdgrp>
    <trd lng="nl">baren</trd>
    <trdgrp lng="pt">
      <trd>parir</trd>,
      <trd>dar &agrave; luz</trd>,
      <trd>ter <klr>filho</klr></trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malfacile produkti verkon:
      <ekz>
        li <tld/>is anta&ubreve; nelonge dikan tezon.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>accoucher <klr>(de quelque chose)</klr></trd>,
      <trd>pondre <klr>(fam.)</klr></trd>
    </trdgrp>
    <trd lng="nl">ter <ind>wereld</ind> brengen</trd>
  </snc>
  <trd lng="de">niederkommen</trd>
  <trd lng="en">be in labor</trd>
  <trd lng="es">dar a luz</trd>
  <trd lng="ru">&c_r;&c_o;&c_zh;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="akusx.0igi">
  <kap><tld/>igi<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Helpi la <tld/>on de virino.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_m;&c_a;&c_c;&c_mol; &c_r;&c_o;&c_d;&c_y;</trd>,
    <trd>&c_b;&c_a;&c_b;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="fr">accoucher <klr>(une femme)</klr></trd>
  <trd lng="pt">partejar</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol; <ind>&c_r;&c_o;&c_d;&c_y;</ind></trd>
</drv>

<drv mrk="akusx.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="forcep.0o">Forcepso</ref>.
    </dif>
  </snc>
  <trd lng="ru">&c_a;&c_k;&c_u;&c_sh;&c_je;&c_r;&c_s;&c_k;&c_i;&c_je; &c_shch;&c_i;&c_p;&c_c;&c_y;</trd>
</drv>

<drv mrk="akusx.0istino">
  <kap><ofc>*</ofc> <tld/>istino</kap>
  <snc>
    <dif>
      Virino, kies profesio estas zorgi la aku&scirc;ojn.
    </dif>
  </snc>
  <ref tip="sin" cel="akusx.0igistino">akusx.0igistino</ref>
  <trd lng="be">&c_a;&c_k;&c_u;&c_sh;&c_e;&c_r;&c_k;&c_a;</trd>
  <trd lng="es">partera</trd>
  <trdgrp lng="fr">
    <trd>sage-femme</trd>,
    <trd>accoucheuse</trd>
  </trdgrp>
  <trd lng="nl">vroedvrouw</trd>
  <trdgrp lng="pt">
    <trd>parteira</trd>,
    <trd>obstetriz</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_k;&c_u;&c_sh;&c_je;&c_r;&c_k;&c_a;</trd>
</drv>

<drv mrk="akusx.0igistino">
  <kap><tld/>igistino</kap>
  <ref tip="dif" cel="akusx.0istino">akusx.0istino</ref>
  <trd lng="be">&c_a;&c_k;&c_u;&c_sh;&c_e;&c_r;&c_k;&c_a;</trd>
  <trd lng="es">partera</trd>
  <trdgrp lng="fr">
    <trd>sage-femme</trd>,
    <trd>accoucheuse</trd>
  </trdgrp>
  <trd lng="nl">vroedvrouw</trd>
  <trdgrp lng="pt">
    <trd>parteira</trd>,
    <trd>obstetriz</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_k;&c_u;&c_sh;&c_je;&c_r;&c_k;&c_a;</trd>
</drv>

<drv mrk="akusx.0intino">
  <kap><tld/>intino</kap>
  <snc>
    <dif>
      Virino, kiu &jcirc;us <tld/>is.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_d;&c_z;&c_ib;&c_h;&c_a;</trd>
  <trd lng="fr">accouch&eacute;e</trd>
  <trd lng="pt">parturiente</trd>
  <trd lng="ru">&c_r;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_c;&c_a;</trd>
</drv>
</art>
