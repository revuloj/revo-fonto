<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vulv.xml,v 1.31 2023/10/21 16:50:36 revo Exp $">
<kap>
  <ofc>9</ofc><rad>vulv</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="vulv.0o">
  <kap><tld/>o</kap>
  <snc mrk="vulv.0o.ANA">
  <uzo tip="fak">ANA</uzo>
    <dif>
      La ekstera&jcirc;o de la seksaj organoj &ccirc;e virino kaj
      &ccirc;e aliaj superaj animalinoj:
      <ekz>
        post kelkaj jaroj komencos kreski &scirc;iaj mamoj, &scirc;ia
        <tld/>o
        pligrandi&gcirc;os
        <fnt>
          <aut>Donald Broadribb</aut>
          <vrk>Ne al &ccirc;iuj pla&ccirc;os &ccirc;i literatura
          gemeto</vrk>
          <lok><url ref="&Monato;/2003/007837.html">Monato</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        apartaj tipoj de la homaj papilom-virusoj povas ka&ubreve;zi malignajn tumorojn &ndash; plej
        ofte kanceron de la utercerviko, sed anka&ubreve; de la anuso, peniso, <tld/>o kaj
        e&ccirc; de faringo
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="https://www.monato.be/2010/010159.php">Vakcino preventas
          kancerojn</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_l;&c_mol;&c_v;&c_a;</trd>
  <trd lng="bg">&c_v;&c_u;&c_l;&c_v;&c_a;</trd>
  <trd lng="ca">vulva</trd>
  <trdgrp lng="cs">
    <trd>stydk&eacute; pysky</trd>,
    <trd>vulva</trd>,
    <trd>&zcaron;ensk&eacute; vn&ecaron;j&scaron;&iacute; pohl.
    &uacute;stroj&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Vulva</trd>,
    <trd>weibliche <ind>Scham</ind></trd>
  </trdgrp>
  <trd lng="en">vulva</trd>
  <trd lng="es">vulva</trd>
  <trd lng="fa">&a_f;&a_fatha;&a_r;&a_j;</trd>
  <trd lng="fr">vulve</trd>
  <trd lng="hu">szem&eacute;remtest</trd>
  <trd lng="io">vulvo</trd>
  <trd lng="it">vulva</trd>
  <trdgrp lng="ja">
    <trd>&#38512;&#38272;  <pr>&#12356;&#12435;&#12418;&#12435;</pr></trd>,
    <trd>&#22806;&#38512;&#x90E8;  <pr>&#x304C;&#12356;&#12356;&#12435;&#12406;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vulva</trd>,
    <trd>schaamspleet</trd>
  </trdgrp>
  <trd lng="pl">srom</trd>
  <trd lng="pt">vulva</trd>
  <trd lng="ru">&c_v;&c_u;&c_l;&c_mol;&c_v;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>vonkaj&scaron;ia &ccaron;as&tcaron; &zcaron;ensk&yacute; pohl.
    org&aacute;nov</trd>,
    <trd>vulva</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_u;&c_l;&c_mol;&c_v;&c_a;</trd>,
    <trd>&c_zh;&c_ib;&c_n;&c_o;&c_ch;&c_i;&c_j; &c_s;&c_t;&c_a;&c_t;&c_je;&c_v;&c_i;&c_j; &c_o;&c_r;&c_g;&c_a;&c_n;</trd>
  </trdgrp>
</drv>

<drv mrk="vulv.0a">
  <kap><tld/>a</kap>
  <snc mrk="vulv.0a.ANA">
    <dif>
      Rilata al vulvo:
      <ekz>
        la grandaj vulvaj lipoj
        <fnt>
          <aut>Vikipedio</aut>,
          <vrk><url ref="&Viki;/Vulvo">Vulvo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">vulvaire</trd>
  <trd lng="io">vulvala</trd>
  <trdgrp lng="ja">
    <trd>&#38512;&#38272;&#x306E;  <pr>&#x304B;&#12370;&#12418;&#12435;&#x306E;</pr></trd>,
    <trd>&#22806;&#38512;&#x90E8;&#x306E; <pr>&#12364;&#12356;&#12356;&#12435;&#12406;&#12398;</pr></trd>
  </trdgrp>
</drv>
</art>
