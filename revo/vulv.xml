<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>9</ofc><rad>vulv</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="vulv.0o">
  <kap><tld/>o</kap>
  <snc mrk="vulv.0o.ANA">
  <uzo tip="fak">ANA</uzo>
    <dif>
      La ekstera&jcirc;o de la seksaj organoj &ccirc;e virino kaj &ccirc;e aliaj superaj
      animalinoj:
      <ekz>
        post kelkaj jaroj komencos kreski &scirc;iaj mamoj, &scirc;ia <tld/>o
        pligrandi&gcirc;os
        <fnt>
        <aut>Donald Broadribb</aut>
        <vrk>Ne al &ccirc;iuj pla&ccirc;os &ccirc;i literatura gemeto</vrk>
        <lok><url
        ref="http://www.esperanto.be/fel/2003/007837.html">Monato</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_l;&c_mol;&c_v;&c_a;</trd>
  <trd lng="ca">vulva</trd>
  <trdgrp lng="cs">
    <trd>stydk&eacute; pysky</trd>,
    <trd>vulva</trd>,
    <trd>&zcaron;ensk&eacute; vn&ecaron;j&scaron;&iacute; pohl. &uacute;stroj&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Vulva</trd>,
    <trd>weibliche Scham</trd>
  </trdgrp>
  <trd lng="en">vulva</trd>
  <trd lng="es">vulva</trd>
  <trd lng="fa">&a_f;&a_fatha;&a_r;&a_j;</trd>
  <trd lng="fr">vulve</trd>
  <trd lng="hu">szem&eacute;remtest</trd>
  <trd lng="io">vulvo</trd>
  <trd lng="it">vulva</trd>
  <trdgrp lng="nl">
    <trd>vulva</trd>,
    <trd>schaamspleet</trd>
  </trdgrp>
  <trd lng="pl">srom</trd>
  <trd lng="pt">vulva</trd>
  <trd lng="ru">&c_v;&c_u;&c_l;&c_mol;&c_v;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>vonkaj&scaron;ia &ccaron;as&tcaron; &zcaron;ensk&yacute; pohl. org&aacute;nov</trd>,
    <trd>vulva</trd>
  </trdgrp>
</drv>

<drv mrk="vulv.0a">
  <kap><tld/>a</kap>
  <snc mrk="vulv.0a.ANA">
    <dif>
      Rilata al vulvo:
      <ekz>
        la grandaj vulvaj lipoj
        <fnt>
          <aut>Vikipedio</aut>,
          <vrk><url
          ref="http://eo.wikipedia.org/wiki/Vulvo">Vulvo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">vulvaire</trd>
  <trd lng="io">vulvala</trd>
</drv>
</art>
