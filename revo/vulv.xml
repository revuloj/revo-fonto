    <trd>vulva</trd>,
    <trd>&zcaron;ensk&eacute; vn&ecaron;j&scaron;&iacute; pohl.
    &uacute;stroj&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Vulva</trd>,
    <trd>weibliche <ind>Scham</ind></trd>
  </trdgrp>
