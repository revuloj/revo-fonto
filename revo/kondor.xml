<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>kondor</rad>/o
</kap>
<drv mrk="kondor.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de la genro <ref tip="super"
      cel="vultur.0o.katartedo">vulturo</ref>
      (<trd lng="la">Vultur gryphus</trd>)
      tre granda rabobirdo, vivanta en Sudameriko.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_n;&c_d;&c_a;&c_r;</trd>
  <trd lng="ca">c&ograve;ndor</trd>
  <trd lng="de">Kondor</trd>
  <trd lng="en">condor</trd>
  <trd lng="es">c&oacute;ndor</trd>
  <trd lng="fr">condor</trd>
  <trd lng="nl">condor</trd>
  <trd lng="pl">kondor</trd>
  <trdgrp lng="pt">
    <trd>condor <klr tip="amb">(&ocirc;)</klr></trd>,
    <trd>abutre-do-novo-mundo</trd>,    
    <trd>condor-dos-andes</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_d;&c_o;&c_r;</trd>
</drv>
</art>
