    <trd>&#x968E;&#x6BB5;&#12373;&#12376;&#x304D;  <pr>&#x304B;&#12356;&#12384;&#12435;&#12373;&#12376;&#x304D;</pr></trd>,
    <trd>&#x968E;&#x6BB5;&#25945;&#x5BA4;  <pr>&#x304B;&#12356;&#12384;&#12435;&#x304D;&#12423;&#12358;&#12375;&#12388;</pr></trd>
  </trdgrp>
  <trd lng="nl">amfitheater</trd>
  <trd lng="pl">amfiteatr</trd>
  <trd lng="ru">&c_a;&c_m;&c_f;&c_i;&c_t;&c_je;&c_a;&c_t;&c_r;</trd>
