<art mrk="$Id: amfite.xml,v 1.28 2024/08/26 01:47:13 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>amfiteatr</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="amfite.0o">
  <kap><tld/>o</kap>
  <snc mrk="amfite.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">TEA</uzo>
    <dif>
      Ronda a&ubreve; duonronda <ref tip="super" cel="konstr.0ajxo.domo">konstrua&jcirc;o</ref>, kun multeta&gcirc;aj
      benkoj,
      por spektantoj, a&ubreve;skultantoj...:
      <ekz>
        ni vidas plenan <tld/>on, la Nuboj de Aristofano eligas torentojn da moko kaj da
        gajeco en la homan amasegon
        <fnt>
          <bib>Fab3</bib>
          <lok>Dorna vojo de la honoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Ursus</nom> staris en la mezo de l&#8217; <tld/>o, nuda <klr>[&#8230;]</klr>, vidante,
        ke la <ref tip="prt" cel="aren.0o.SPO">areno</ref>
        estas malplena, li mire rigardis <klr>[&#8230;]</klr> jen la publikon, jen Cezaron,
        jen la kradojn <klr>[&#8230;]</klr> de kie li atendis siajn ekzekutistojn
        <fnt>
          <bib>IKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        sur la deklivoj <tld/>e ku&scirc;as urbeto
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 17</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="auxdit.0o">a&ubreve;ditorio</ref>,
       <ref cel="cirk.0o.ARKI">cirko<sncref/></ref>,
       <ref cel="teatr.0ejo">teatrejo</ref>,
       <ref cel="tribun1.0o.jugxejo">tribunalo<sncref/></ref>
    </refgrp>
    <bld lok="&WCU;/thumb/5/54/Pompeji_-_Arena.jpg/640px-Pompeji_-_Arena.jpg"
    prm="CC-BY-SA-3.0"><tld/>o de Pompejo
        <fnt>
          <aut>Buckeye</aut>
          <lok><url ref="&WCW;/File:Pompeji_-_Arena.jpg">&WCW;/File:Pompeji_-_Arena.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="zh">
    <trd>&#x7ADE;&#25216;&#x573A; <pr>j&#xEC;ngj&#xEC;ch&#x1CE;ng</pr></trd>,
    <trd>&#x7AF6;&#25216;&#22580; <pr>j&#xEC;ngj&#xEC;ch&#x1CE;ng</pr></trd>,
    <trd>&#x821E;&#x53F0; <pr>w&#x1D4;t&#xE1;i</pr></trd>,
    <trd>&#34945;&#x884C;&#38706;&#22825;&#21095;&#x573A; <pr>yu&#xE1;nx&#xED;ngl&#xF9;ti&amacron;nj&#xF9;ch&#x1CE;ng</pr></trd>,
    <trd>&#34945;&#x884C;&#38706;&#22825;&#21127;&#22580; <pr>yu&#xE1;nx&#xED;ngl&#xF9;ti&amacron;nj&#xF9;ch&#x1CE;ng</pr></trd>,
    <trd>&#38454;&#x68AF;&#x5F0F;&#x5EA7;&#x4F4D;&#x533A;
    <pr>ji&emacron;t&#x12B;sh&#xEC;zu&#xF2;w&#xE8;iq&#x16B;</pr></trd>,
    <trd>&#x968E;&#x68AF;&#x5F0F;&#x5EA7;&#x4F4D;&#21312;
    <pr>ji&emacron;t&#x12B;sh&#xEC;zu&#xF2;w&#xE8;iq&#x16B;</pr></trd>,
    <trd>&#26007;&#x517D;&#x573A; <pr>d&#xF2;ush&#xF2;uch&#x1CE;ng</pr></trd>,
    <trd>&#x9B25;&#29560;&#22580; <pr>d&#xF2;ush&#xF2;uch&#x1CE;ng</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_m;&c_f;&c_ib;&c_t;&c_e;&c_a;&c_t;&c_a;&c_r;</trd>
  <trd lng="ca">amfiteatre</trd>
  <trd lng="cs">amfite&aacute;tr</trd>
  <trd lng="de">Amphitheater</trd>
  <trd lng="en">amphitheater</trd>
  <trd lng="es">anfiteatro</trd>
  <trd lng="fa">&a_A_madda;&a_m;&a_f;&f_y;&#8204;&a_t;&a_A_madda;&a_t;&a_r;</trd>
  <trd lng="fr">amphith&eacute;&acirc;tre</trd>
  <trd lng="he">&alef;&mem;&pe;&yod;&tav;&alef;&tet;&resh;&vav;&fnun;</trd>
