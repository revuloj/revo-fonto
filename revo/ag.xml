    <trd>bezczynno&sacute;&cacute;</trd>
  </trdgrp>
  <trd lng="pt">inatividade</trd>
  <trd lng="ru">
    &c_b;&c_je;&c_z;&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;
  </trd>
  <trdgrp lng="sk">
    <trd>ne&ccaron;innos&tcaron;</trd>,
    <trd>z&aacute;ha&lcaron;ka</trd>
  </trdgrp>
  <trd lng="sv">inaktivitet</trd>
  <trd lng="th">&#xE44;&#xE21;&#xE48;&#xE17;&#xE33;&#xE2D;&#xE30;&#xE44;&#xE23;&#xE40;&#xE25;&#xE22;</trd>
  <trdgrp lng="tr">
