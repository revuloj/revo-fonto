<drv mrk="ag.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ag.0i.eksterigi">
    <dif>
      Eksterigi siajn kapablojn, produkti ian efikon, uzante sian forton
      a&ubreve; kapablon:
      <ekz>
        honesta homo <tld/>as honeste
        <fnt>
          <bib>F</bib>
          <lok>&FE; 25</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i juste
        <fnt><bib>MT</bib> <lok>&Mih; 6:8</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi ne povis <tld/>i pli delikate
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino kaj Rozulino</lok>
        </fnt>?
      </ekz>
      <ekz>
        ni <tld/>as ankora&ubreve; blinde kaj riskas, ke nia laborado pereos vane
        <fnt>
          <bib>EE</bib>
          <lok>I</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne devas <tld/>i kontra&ubreve; mia konscienco
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi devas <tld/>i, anta&ubreve; ol tiu &ccirc;i &ccirc;iutaga kuneestado renovi&gcirc;os
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
<!--
fontoj...?
      <ekz>
        plena libereco de <ind><tld/>ado</ind><fnt>Z</fnt>;
        <trdgrp lng="be">
          <trd>&c_d;&c_z;&c_je;&c_ja;&c_n;&c_mol;&c_n;&c_ib;</trd>,
          <trd>&c_d;&c_z;&c_je;&c_j;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
          <trd>&c_ch;&c_y;&c_n;&c_n;&c_a;&c_s;&c_c;&c_mol;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        ilia <tld/>ado estas dan&gcirc;erega por nia afero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>ada rolo<fnt>Z</fnt>;
      </ekz>
-->
      <ekz>
        pli facile estas regi ol <tld/>i
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu <tld/>as afable, vivas agrable
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="far.0i.iel_agi">fari<sncref/></ref>,
      <ref cel="proced.0i">procedi</ref>
    </refgrp>
    <trdgrp lng="es">
      <trd>actuar</trd>,
      <trd>proceder</trd>,
      <trd>obrar</trd>
    </trdgrp>
    <trd lng="ja">&#x884C;&#x52D5;&#x3059;&#x308B; <pr>&#x3053;&#x3046;&#x3069;&#x3046;&#x3059;&#x308B;</pr></trd>
    <trdgrp lng="pl">
      <trd>dzia&lstroke;a&cacute;</trd>,
      <trd>czyni&cacute;</trd>,
      <trd>post&eogonek;powa&cacute;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>agir</trd>,
      <trd>proceder</trd>,
      <trd>obrar</trd>
    </trdgrp>
    <trd lng="th">&#xE01;&#xE23;&#xE30;&#xE17;&#xE33;</trd>
  </snc>
  <snc mrk="ag.0i.trakti">
    <dif>
      <ref cel="kondut.0i">Konduti</ref> iel kontra&ubreve; iu,
      <ref tip="vid" cel="trakt.0i.sinteni">trakti</ref> iun:
      <ekz>
        li scii&gcirc;is, kiel <tld/>is kun li lia pli juna filo
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 9:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        se vi <tld/>os malbone kontra&ubreve; miaj filinoj <klr>[&#x2026;]</klr>, tiam <klr>[&#x2026;]</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 31:50</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u kun aliaj tiel, kiel vi deziras ke aliaj <tld/>u kun vi
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Homaranismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        kiel pardoneme la ju&gcirc;o <tld/>as kun vi, malvirtulo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="es">
      <trd>actuar</trd>,
      <trd>proceder</trd>
    </trdgrp>
    <trd lng="ja">&#x3075;&#x308B;&#x307E;&#x3046;</trd>
    <trdgrp lng="pl">
      <trd>czyni&cacute;</trd>,
      <trd>post&eogonek;powa&cacute;</trd>
    </trdgrp>
    <trdgrp lng="pt">
