<drv mrk="ag.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ag.0i.eksterigi">
    <dif>
      Eksterigi siajn kapablojn, produkti ian efikon, uzante sian forton
      a&ubreve; kapablon:
      <ekz>
        <tld/>i juste
        <fnt><bib>MT</bib> <lok>&Mih; 6:8</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i noble, nesingarde, kun kolero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        vortojn &scirc;paru, <tld/>ojn faru<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        nenia <tld/>o fari&gcirc;as sen pago
        <klr>(sen bona a&ubreve; malbona re<tld/>o)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        plena libereco de <ind><tld/>ado</ind><fnt>Z</fnt>;
        <trdgrp lng="be">
          <trd>&c_d;&c_z;&c_je;&c_ja;&c_n;&c_mol;&c_n;&c_ib;</trd>,
          <trd>&c_d;&c_z;&c_je;&c_j;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
          <trd>&c_ch;&c_y;&c_n;&c_n;&c_a;&c_s;&c_c;&c_mol;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        ilia <tld/>ado estas dan&gcirc;erega por nia afero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>ada rolo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="es">
      <trd>actuar</trd>,
      <trd>proceder</trd>,
      <trd>obrar</trd>
    </trdgrp>
    <trd lng="ja">&#x884C;&#x52D5;&#x3059;&#x308B; [<ind>&#x3053;&#x3046;&#x3069;&#x3046;&#x3059;&#x308B;</ind>]</trd>
    <trdgrp lng="pl">
      <trd>dzia&lstroke;a&cacute;</trd>,
      <trd>czyni&cacute;</trd>,
      <trd>post&eogonek;powa&cacute;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>agir</trd>,
      <trd>proceder</trd>,
      <trd>obrar</trd>
    </trdgrp>
    <trd lng="th">&#xE01;&#xE23;&#xE30;&#xE17;&#xE33;</trd>
  </snc>
  <snc mrk="ag.0i.trakti">
    <dif>
      Konduti iel kontra&ubreve; iu,
      <ref tip="vid" cel="trakt.0i.sinteni">trakti</ref> iun:
      <ekz>
        <tld/>i afable kun, kontra&ubreve;<fnt>Z</fnt> iu.
      </ekz>
    </dif>
    <trdgrp lng="es">
      <trd>actuar</trd>,
      <trd>proceder</trd>
    </trdgrp>
    <trd lng="ja">&#x3075;&#x308B;&#x307E;&#x3046;</trd>
    <trdgrp lng="pl">
      <trd>czyni&cacute;</trd>,
      <trd>post&eogonek;powa&cacute;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>agir</trd>,
      <trd>proceder</trd>
    </trdgrp>
    <trd lng="th">&#xE1B;&#xE0F;&#xE34;&#xE1A;&#xE31;&#xE15;&#xE34;</trd>
  </snc>
  <trd lng="af">Wet</trd>
  <trd lng="am">&#x12A5;&#x122D;&#x121D;&#x1303;</trd>
  <trd lng="ar">&#x641;&#x639;&#x644;</trd>
  <trd lng="az">akt</trd>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_je;&c_j;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_ch;&c_y;&c_n;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_a;&c_m;</trd>
  <trd lng="bn">&#x986;&#x987;&#x9A8;</trd>
  <trdgrp lng="br">
    <trd>ober<klr tip="ind"> (v.)</klr></trd>,
    <trd>gweredi&ntilde; war</trd>
