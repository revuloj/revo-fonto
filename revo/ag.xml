    <trd>&a_f;&a_ayn;&a_A;&a_l;</trd>
  </trdgrp>
  <trd lng="fr">actif</trd>
  <trd lng="he">&pe;&ayin;&yod;&lamed;</trd>
  <trdgrp lng="it">
    <trd>attivo</trd>,
    <trd>d'<ind>azione</ind></trd>,
    <trd>alcre</trd>,
    <trd>solerte</trd>
