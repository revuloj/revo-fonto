  </snc>
  <trd lng="be">&c_r;&c_e;&c_a;&c_k;&c_c;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_r;&c_je;&c_a;&c_k;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>ohlas</trd>,
    <trd>reakce</trd>
  </trdgrp>
  <trd lng="de">Reaktion</trd>
