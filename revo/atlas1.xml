<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: atlas1.xml,v 1.4 2023/10/13 17:08:38 afido Exp $">
    <kap>
      <ofc>2</ofc>
      <rad>atlas</rad>/o
    </kap>

<drv mrk="atlas1.0o">
  <kap><tld/>o</kap>
  <snc mrk="atlas1.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Luksa, dol&ccirc;e mola, brila kaj glata
      <ref tip="super" cel="sxtof.0o.TEKS">&scirc;tofo</ref>, silka
      <ref tip="super" cel="saten.0o.cxia">sateno</ref>:
      <ekz>
        meblaj kovriloj estis el blua <tld/>o
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili eniris en la unuan salonon, &gcirc;i estis kovrita per rozokolora <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        moda vesto el violkolora <tld/>o, kun multe da veluro kaj fran&gcirc;oj de tia sama
        koloro
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la virino en la <tld/>a&jcirc;oj ekridis, kaj, turnante sin al sia kunulo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        da puntoj, rubandoj, tuloj, gazoj, ka&scirc;miroj, <tld/>oj
        multe estis en la &scirc;rankoj kaj komodoj
        <fnt>
          <aut>E. Orzeszko, trad. Kabe</aut>,
          <vrk>La bona sinjorino</vrk>
        </fnt>;
      </ekz>
      <ekz>
        Zonjo surmetas <klr>[&#x2026;]</klr> la var&scirc;avajn, blankajn, <tld/>ajn &scirc;uetojn
        <fnt>
          <bib>SrT</bib>
          <lok>Libro V.</lok>
        </fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a ha&ubreve;to.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_t;&c_l;&c_a;&c_s; <klr tip="amb">(&c_t;&c_k;&c_a;&c_n;&c_ib;&c_n;&c_a;)</klr></trd>,
      <trd>&c_s;&c_a;&c_c;&c_ib;&c_n;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Satin</trd>,
      <trd>Atlasseide</trd>,
      <trd><ofc>2</ofc>Atlask&ouml;per</trd>
    </trdgrp>
    <trd lng="en"><ofc>2</ofc>satin</trd>
    <trdgrp lng="fr">
      <trd><ofc>2</ofc>satin</trd>,
      <trd>atlas <klr>(tissu)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>atlasz (kelme)</trd>,
      <trd>szat&eacute;n</trd>
    </trdgrp>
    <trd lng="it">satin</trd>
    <trdgrp lng="nl">
      <trd>atlas</trd>,
      <trd>satijn</trd>
    </trdgrp>
    <trd lng="pl">at&lstroke;as</trd>
    <trd lng="pt">cetim</trd>
    <trd lng="ru">&c_a;&c_t;&c_l;&c_a;&c_s;<klr tip="amb"> [&c_a;&c_t;&c_l;&aacute;&c_s;]</klr></trd>
    <trdgrp lng="sv">
      <trd>sat&auml;ng</trd>,
      <trd>satin</trd>
    </trdgrp>
  </snc>
  <ref tip="hom" cel="atlas.0o.maparo">atlaso (maparo)</ref>
  <trdgrp lng="ja">
    <trd>&#x5730;&#x56F3;&#x5E33;  <pr>&#x3061;&#x305A;&#x3061;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x56F3;&#x8868;&#x96C6;  <pr>&#x305A;&#x3072;&#x3087;&#x3046;&#x3057;&#x3085;&#x3046;</pr></trd>
  </trdgrp>
</drv>

  </art>

