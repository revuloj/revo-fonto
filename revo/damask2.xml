<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: damask2.xml,v 1.6 2022/04/26 17:11:47 revo Exp $">
<kap>
    <rad>Damask</rad>/o <fnt><bib>MT</bib></fnt>
</kap>
<drv mrk="damask2.D0o">
  <kap><tld lit="D"/>o</kap>
  <snc mrk="damask2.D0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&Ccirc;efurbo</ref> 
      kaj la plej granda urbo de <ref tip="malprt" cel="siri.S0o">Sirio</ref>:
      <ekz>
        re&gcirc;o de Sirio, kiu lo&gcirc;is en <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 15:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        via nazo estas kiel la turo de Lebanon rigardanta <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 7:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        la urbo de la <tld/>anoj
        <fnt>
          <bib>NT</bib>
          <lok>II. Korintanoj 11:32</lok>
        </fnt>;
      </ekz>
      <ekz>
        la roz&gcirc;ardenoj de <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fulmo trafis Pa&ubreve;lon survoje al <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Sen Rodin</aut>
          <vrk><url ref="&Monato;/2012/010648.php"
              >Papilia efiko</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="damask.0o">damasko</ref>
  <trd lng="be">&c_D;&c_a;&c_m;&c_a;&c_s;&c_k;</trd>
  <trd lng="bg">&c_D;&c_a;&c_m;&c_a;&c_s;&c_k;</trd>
  <trd lng="ca">Damasc</trd>
  <trd lng="de">Damaskus</trd>
  <trd lng="en">Damascus</trd>
  <trd lng="fr">Damas</trd>
  <trd lng="nl">Damascus</trd>
  <trd lng="pl">Damaszek</trd>
  <trd lng="sv">Damaskus</trd>
  <trd lng="sk">Dama&scaron;ek</trd>
  <trd lng="cs">Dama&scaron;ek</trd>
</drv>
</art>

