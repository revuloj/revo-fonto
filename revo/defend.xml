  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="defend.0ajxo.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Objekto uzata por <tld/>i:
      <ekz>
        pordego <klr>...</klr> funkcias kiel la lasta <tld/>a&jcirc;o de la 
        urbo
        <fnt>
          <aut>Yi Jun</aut>,
          <vrk><url ref="http://www.espero.com.cn/old/epch/z05-07k/07-3m/05z-07k-3-2.htm">
          Urbopordegoj de Pekino</url></vrk>,
          <lok>Espero.com.cn, [2009?]</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="blend1.0o">blendo</ref>,
      <ref cel="fortik.0ajxo">fortika&jcirc;o</ref>,
      <ref cel="rempar.0o.ARKI">remparo<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_a;&c_r;&c_o;&c_n;&c_ch;&c_y; &c_s;&c_r;&c_o;&c_d;&c_a;&c_k;</trd>,
    <trd>&c_a;&c_b;&c_a;&c_r;&c_o;&c_n;&c_ch;&c_a;&c_je; &c_p;&c_r;&c_y;&c_s;&c_t;&c_a;&c_s;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_a;&c_b;&c_a;&c_r;&c_o;&c_n;&c_ch;&c_a;&c_je; &c_z;&c_b;&c_u;&c_d;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="de">Schutz</trd>
  <trd lng="fr">dispositif de <ind>d&eacute;fense</ind></trd>
  <trd lng="zh">&#x9632;&#x536B;&#x7269; <pr>f&#xE1;ngw&#xE8;iw&#xF9;</pr></trd>
</drv>

<drv mrk="defend.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="defend.cxirkaux0i.KOMUNE">
    <dif>
      <tld lit="D"/>i &ccirc;iuflanke:
      <ekz>
        kiel per &scirc;ildo Vi &ccirc;irka&ubreve;<tld/>as lin per
        favoro
        <fnt><bib>MT</bib><lok>&Psa; 5:12</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_a;&c_r;&c_a;&c_n;&c_ja;&c_c;&c_mol; &c_z; &c_u;&c_s;&c_ib;&c_h; &c_b;&c_a;&c_k;&c_o;&c_w;</trd>
  <trd lng="fr">d&eacute;fendre<klr tip="ind"> (de toutes
  parts)</klr></trd>
</drv>

<drv mrk="defend.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="defend.sen0a.KOMUNE">
    <dif>
      Kiu malhavas <tld/>on, protekton, &scirc;irmon,
      kiu estas <ref tip="vid" cel="help.sen0a.iu">senhelpa</ref>:
      <ekz>
        mi estas tute sen<tld/>a, infano povas min renversi
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        simile al sen<tld/>a &scirc;afo mi klinas mian nukon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la koro <klr>[&#x2026;]</klr> estas &ccirc;iam tre sentema kaj samtempe tute sen<tld/>a loko
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili vidis, ke juna, sen<tld/>a virino estas la sola persono, kiun ili atakas
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_z;&c_a;&c_b;&c_a;&c_r;&c_o;&c_n;&c_n;&c_y;</trd>
  <trd lng="cs">bezbrann&yacute;</trd>
  <trd lng="de">wehrlos</trd>
  <trdgrp lng="en">
    <trd>defenseless</trd>,
    <trd>defenceless</trd>,
    <trd>vulnerable</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>vuln&eacute;rable</trd>,
    <trd>sans <ind>d&eacute;fense</ind></trd>
  </trdgrp>
  <trd lng="hu">v&eacute;dtelen</trd>
  <trd lng="ja">&#x7121;&#x9632;&#x5099;&#x306E;  <pr>&#x3080;&#x307C;&#x3046;&#x3073;&#x306E;</pr></trd>
  <trd lng="nl">kwetsbaar</trd>
  <trd lng="pl">bezbronny</trd>
  <trd lng="sk">bezbrann&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_je;&c_z;&c_z;&c_a;&c_h;&c_i;&c_s;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_b;&c_o;&c_r;&c_o;&c_n;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6C92;&#x6709;&#x9632;&#x5099;&#x80FD;&#x529B; <pr>m&#xE9;iy&#x1D2;uf&#xE1;ngb&#xE8;in&#xE9;ngl&#xEC;</pr></trd>,
    <trd>&#x8D64;&#x624B;&#x7A7A;&#x62F3; <pr>ch&#xEC;sh&#x1D2;uk&#x14D;ngqu&#xE1;n</pr></trd>,
    <trd>&#x624B;&#x65E0;&#x5BF8;&#x94C1; <pr>sh&#x1D2;uw&#xFA;c&#xF9;nti&#x11B;</pr></trd>,
    <trd>&#x4E0D;&#x8A2D;&#x9632; <pr>b&#xF9;sh&#xE8;f&#xE1;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="defend.sin0o">
  <kap>sin<tld/>o</kap>
  <snc mrk="defend.sin0o.KOMUNE">
    <dif>
      <tld lit="D"/>o de si mem kontra&ubreve; eksteraj atakoj:
      <ekz>
        granda tran&ccirc;ilo por sin<tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>
      <ekz>
        longaj akuzoj kaj egale longaj sin<tld/>oj
        <fnt>
          <aut>K. Miner</aut>,
          <vrk><url ref="http://www.sunflower.com/~miner/TAGLIBRO_package/Auxgusto_2006.html">
          Taglibro</url></vrk>,
          <lok>2006-08-02</lok>
        </fnt>;
      </ekz>
      <ekz>
        se estas virto rezisti fizike kontra&ubreve; fizikaj atakoj, des pli estas aprobinda la
        mensa sin<tld/>o
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_m;&c_a;&c_a;&c_b;&c_a;&c_r;&c_o;&c_n;&c_a;</trd>
  <trd lng="cs">sebeobrana</trd>
  <trdgrp lng="de">
    <trd>Selbstverteidigung</trd>,
    <trd>Selbstschutz</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;fense de soi</trd>
  <trd lng="ja">&#x81EA;&#x5DF1;&#x9632;&#x885B;  <pr>&#x3058;&#x3053;&#x307C;&#x3046;&#x3048;&#x3044;</pr></trd>
