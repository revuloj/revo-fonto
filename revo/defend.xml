<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>defend</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="defend.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="defend.0i.KOMUNE">
    <dif>
      Helpi, protekti iamaniere kontra&ubreve; atako:
      <ekz>
        <tld/>i urbon kontra&ubreve; la sie&gcirc;antoj;
      </ekz>
      <ekz>
        <tld/>i per armiloj, agoj, parolado;
      </ekz>
      <ekz>
        li <tld/>is sin mem, sen ia helpo;
      </ekz>
      <ekz>
        ne <tld/>as oro kontra&ubreve; doloro<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la tasko de advokato estas <tld/>i la
        akuzitojn anta&ubreve; la tribunalo;
      </ekz>
      <ekz>
        la delegito fervore
        <tld/>is la projekton<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i opinion, pretendojn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pled.0i">pledi</ref>,
      <ref cel="protek.0i.sxirmi">protekti</ref>,
      <ref cel="sxirm.0i.protekti">&scirc;irmi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_a;&c_r;&c_a;&c_n;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_h;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd>&c_b;&c_a;&c_r;&c_a;&c_n;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">verteidigen</trd>
  <trd lng="en">defend</trd>
  <trd lng="es">defender</trd>
  <trd lng="fr">d&eacute;fendre<klr tip="ind"> (contre les
  attaques)</klr></trd>
  <trdgrp lng="hu">
    <trd>v&eacute;d</trd>,
    <trd>megv&eacute;d</trd>
  </trdgrp>
  <trd lng="pl">broni&cacute;</trd>
  <trdgrp lng="pt">
    <trd>defender</trd>,
    <trd>proteger</trd>,
    <trd>resguardar</trd>,
    <trd>pugnar por</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_shch;&c_i;&c_shch;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>br&aacute;ni&tcaron;</trd>,
    <trd>chr치ni콘</trd>,
    <trd>h치ji콘</trd>
  </trdgrp>
  <trd lng="sv">f&ouml;rsvara</trd>
</drv>

<drv mrk="defend.0o">
  <kap><tld/>o</kap>
  <snc mrk="defend.0o.KOMUNE">
    <dif>
      Ago de tiu, kiu <tld/>as:
      <ekz>
        al li ne mankas <tld/>o kontra&ubreve; ofendo;
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="apolog1.0o">apologio</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_a;&c_r;&c_o;&c_n;&c_a;</trd>,
    <trd>&c_a;&c_h;&c_o;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;fense</trd>
  <trd lng="pl">obrona</trd>
  <trdgrp lng="sk">
    <trd>obhajoba</trd>,
    <trd>obrana</trd>
  </trdgrp>
</drv>

<drv mrk="defend.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="defend.cxirkaux0i.KOMUNE">
    <dif>
      <tld lit="D"/>i &ccirc;iuflanke:
      <ekz>
        kiel per &scirc;ildo Vi &ccirc;irka&ubreve;<tld/>as lin per
        favoro
        <fnt><bib>MT</bib><lok>&Psa; 5:12</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">d&eacute;fendre<klr tip="ind"> (de toutes
  parts)</klr></trd>
</drv>

<drv mrk="defend.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="defend.sen0a.KOMUNE">
    <dif>
      Kiu malhavas <tld/>on, protekton, &scirc;irmon,
      kiu estas
      <ref tip="vid" cel="help.sen0a.iu">senhelpa</ref>:
      <ekz>
        mi estas <ind>sen<tld/>a</ind><fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_z;&c_a;&c_b;&c_a;&c_r;&c_o;&c_n;&c_n;&c_y;</trd>
  <trdgrp lng="fr">
    <trd>vuln&eacute;rable</trd>,
    <trd>sans <ind>d&eacute;fense</ind></trd>
  </trdgrp>
  <trd lng="hu">v&eacute;dtelen</trd>
  <trd lng="pl">bezbronny</trd>
  <trd lng="sk">bezbrann&yacute;</trd>
</drv>

<drv mrk="defend.sin0o">
  <kap>sin<tld/>o</kap>
  <snc mrk="defend.sin0o.KOMUNE">
    <dif>
      <tld lit="D"/>o de si mem kontra&ubreve; eksteraj atakoj:
      <ekz>
        longaj akuzoj kaj egale longaj sin<tld/>oj
        <fnt>
          <aut>K. Miner</aut>,
          <vrk><url
          ref="http://www.sunflower.com/~miner/TAGLIBRO_package/Auxgusto_2006.html">
          Taglibro</url></vrk>,
          <lok>2006-08-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">d&eacute;fense de soi</trd>
  <trd lng="sk">sebaobrana</trd>
</drv>
</art>
