<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>defend</rad>/i
</kap>
<drv mrk="defend.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Helpi, protekti iamaniere kontra&ubreve; atako:
      <ekz>
        <tld/>i urbon kontra&ubreve; la sie&gcirc;antoj;
      </ekz>
      <ekz>
        <tld/>i per armiloj, agoj, parolado;
      </ekz>
      <ekz>
        li <tld/>is sin mem, sen ia helpo;
      </ekz>
      <ekz>
        ne <tld/>as oro kontra&ubreve; doloro
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
      </ekz>
      <ekz>
        la tasko de advokato estas <tld/>i la
        akuzitojn  anta&ubreve; la tribunalo;
      </ekz>
      <ekz>
        la delegito fervore
        <tld/>is la projekton<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i opinion, pretendojn;
      </ekz>
      <ekz>
        al li ne mankas <ind><tld/>o</ind> kontra&ubreve; ofendo
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
        <trd lng="fr">d&eacute;fense</trd>
      </ekz>
      <ekz>
        mi estas <ind>sen<tld/>a</ind><fnt>Z</fnt>.
        <trdgrp lng="fr">
          <trd>vuln&eacute;rable</trd>,
          <trd>sans <ind>d&eacute;fense</ind></trd></trdgrp> 
      </ekz>  
    </dif>
  </snc>
  <trd lng="de">verteidigen</trd>
  <trd lng="en">defend</trd>
  <trd lng="es">defender</trd>
  <trd lng="fr">d&eacute;fendre<klr tip="ind"> (contre les attaques)</klr></trd>
  <trd lng="ru">&c_z;&c_a;&c_shch;&c_i;&c_shch;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">f&ouml;rsvara</trd>
</drv>
</art>
