<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: defend.xml,v 1.32 2023/11/04 13:16:26 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>defend</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="defend.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="defend.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="protek.0i">Protekti</ref> iamaniere kontra&ubreve; atako:
      <ekz>
        per &ccirc;i tiu pre&gcirc;o &scirc;i sin <tld/>is kontra&ubreve; la timoj
        <fnt>
          <bib>Lasta</bib>
          <lok>La Lasta</lok>
        </fnt>;
      </ekz>
      <ekz>
        kontra&ubreve; malfeli&ccirc;o ne <tld/>as e&ccirc; ri&ccirc;o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <subsnc mrk="defend.0i.MIL">
      <uzo tip="fak">MIL</uzo>
      <dif>
        &Scirc;irmi kontra&ubreve; korpa a&ubreve; arma atako:
        <ekz>
          <tld/>i urbon kontra&ubreve; la sie&gcirc;antoj;
        </ekz>
        <ekz>
          lia konduto ne pla&ccirc;is al la rigardantoj <klr>[&#x2026;]</klr>, se la grandegulo sin ne
          <tld/>os, la spektaklo malsukcesos
          <fnt>
            <bib>IKr</bib>
            <lok>Quo Vadis?</lok>
          </fnt>;
        </ekz>
        <ekz>
          balda&ubreve; la malfeli&ccirc;ulo estis atingita, kaptita kaj &jcirc;etita teren malgra&ubreve; kura&gcirc;a
          reatako de li provita por lin <tld/>i
          <fnt>
            <bib>KPr</bib>
            <lok>&Ccirc;apitro Dekoka</lok>
          </fnt>;
        </ekz>
        <ekz>
          Mi en kampo kapon metos, // Por <tld/>i patrolandon
          <fnt>
            <bib>FK</bib>
            <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
          </fnt>;
        </ekz>
        <ekz>
          La floreto batalante // Sin defendis pikadante
          <fnt>
            <bib>FK</bib>
            <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
          </fnt>;
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="defend.0i.JUR">
      <uzo tip="fak">JUR</uzo>
      <dif>
        Dokumente a&ubreve; parole helpi kontra&ubreve; akuzoj:
        <ekz>
          advokato <klr>[&#x2026;]</klr> venis ju&gcirc;ejon, por <tld/>i la aferon de sia kliento
          <fnt>
            <bib>FK</bib>
            <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
          </fnt>;
        </ekz>
        <ekz>
          min mem li elektis kiel advokaton, por lin <tld/>i
          <fnt>
            <bib>ChL</bib>
            <lok>&Ccirc;apitro Kvina</lok>
          </fnt>;
        </ekz>
        <ekz>
          se pekas homo kontra&ubreve; homo, povas <tld/>i lin Dio
          <fnt>
            <bib>MT</bib>
            <lok>I. Samuel 2:25</lok>
          </fnt>;
        </ekz>  
        <ekz>
          se neniu plendas, neniu <tld/>as
          <fnt>
            <bib>PrV</bib>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="defend.0i.ordinare">
      <dif>
        <ref tip="super" cel="argume.0i">Argumenti</ref>, paroli favore al...:
        <ekz>
          kun flama elokventeco li <tld/>as la planon de sia amiko
          <fnt>
            <bib>FK</bib>
            <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
          </fnt>;
        </ekz>
        <ekz>
          mi <tld/>as fervore nian nunan vojon
          <fnt>
            <bib>Paroloj</bib>
            <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista en Washington en la 15a de a&ubreve;gusto
            1910</vrk>
          </fnt>;
        </ekz>
        <ekz>
          mi ne volas en la nuna momento <tld/>i la projekton, pri kiu vi en la plej
          proksimaj tagoj diskutos
          <fnt>
            <bib>Paroloj</bib>
            <vrk>Parolado anta&ubreve; la Sepa Kongreso Esperantista en Antwerpen en la 21a de a&ubreve;gusto
            1911</vrk>
          </fnt>;
        </ekz>
        <ekz>
          la demandoj, kiuj elpu&scirc;i&gcirc;is el &scirc;ia bu&scirc;o, <klr>[&#x2026;]</klr> &scirc;ovi&gcirc;adis anta&ubreve; &scirc;in kiel
          &scirc;ildo, kiu devas <tld/>i &scirc;in kontra&ubreve; &dash; morto per malsato
          <fnt>
            <bib>Marta</bib>
          </fnt>;
        </ekz>
        <ekz>
          <klr>[sian]</klr> starpunkton li <tld/>is en la verko
          <fnt>
            <bib>Ret</bib>
            <lok>Unua Parto</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="pled.0i">pledi</ref>,
      <ref cel="protek.0i.sxirmi">protekti<sncref/></ref>,
      <ref cel="bat.re0i">rebati</ref>,
      <ref cel="sxirm.0i.protekti">&scirc;irmi<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_a;&c_r;&c_a;&c_n;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_h;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd>&c_b;&c_a;&c_r;&c_a;&c_n;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&t_sa;&t_rata;&t_shabchu;&t_nga; &t_ba;&t_sa;&t_rata;&t_shabchu;&t_nga;&t_sa; &t_ba;&t_sa;&t_rata;&t_shabchu;&t_nga; &t_sa;&t_rata;&t_shabchu;&t_nga;&t_sa;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>br&aacute;nit</trd>,
    <trd>chr&aacute;nit</trd>,
    <trd>h&aacute;jit</trd>,
    <trd>obhajovat</trd>,
    <trd>zastat se <klr>(n&ecaron;koho)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verteidigen</trd>,
    <trd>abwehren</trd>
  </trdgrp>
  <trd lng="en">defend</trd>
  <trd lng="es">defender</trd>
  <trd lng="fr">d&eacute;fendre<klr tip="ind"> (contre les
  attaques)</klr></trd>
  <trd lng="he">&lamed;&#x5B0;&he;&#x5B7;&gimel;&#x5B5;&fnun;</trd>
  <trdgrp lng="hu">
    <trd>v&eacute;d</trd>,
    <trd>megv&eacute;d</trd>
  </trdgrp>
  <trd lng="io">defensar</trd>
  <trdgrp lng="ja">
    <trd>&#x5B88;&#x308B;  <pr>&#x307E;&#x3082;&#x308B;</pr></trd>,
    <trd>&#x9632;&#x5FA1;&#x3059;&#x308B;  <pr>&#x307C;&#x3046;&#x304E;&#x3087;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x9632;&#x885B;&#x3059;&#x308B;  <pr>&#x307C;&#x3046;&#x3048;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5F01;&#x8B77;&#x3059;&#x308B;  <pr>&#x3079;&#x3093;&#x3054;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">verdedigen</trd>
  <trd lng="pl">broni&cacute;</trd>
  <trdgrp lng="pt">
    <trd>defender</trd>,
    <trd>proteger</trd>,
    <trd>resguardar</trd>,
    <trd>pugnar por</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_shch;&c_i;&c_shch;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>br&aacute;ni&tcaron;</trd>,
    <trd>chr&aacute;ni&tcaron;</trd>,
    <trd>h&aacute;ji&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">f&ouml;rsvara</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_h;&c_i;&c_shch;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_b;&c_o;&c_r;&c_o;&c_n;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_o;&c_r;&c_o;&c_n;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_s;&c_t;&c_o;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x9632;&#x5FA1; <pr>f&#xE1;ngy&#xF9;</pr></trd>
</drv>

<drv mrk="defend.0o">
  <kap><tld/>o</kap>
  <snc mrk="defend.0o.KOMUNE">
    <dif>
      Ago de tiu, kiu <tld/>as; rimedo por <tld/>i:
      <ekz>
        la du batalantoj <klr>[&#x2026;]</klr> estis tro okupataj de sia propra <tld/>o, por
        rimarki, ke sub iliaj piedoj malfermi&gcirc;as profunda kaj nigra abismo
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        Dio estas mia <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 59:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        en momento kiam la proceso &scirc;ajnis finita majstro Klozelo, advokato de la
        <tld/>o, naskigis &scirc;ancelon
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi estas vere malprava <klr>[&#x2026;]</klr> en via sistemo de <tld/>o
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kupolo <klr>[&#x2026;]</klr> servas precipe kiel <tld/>o kontra&ubreve; la &scirc;an&gcirc;oj de la
        vetero
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o anta&ubreve; tribunalo
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li vekas ankora&ubreve; pli fortan indignon kontra&ubreve; ilin per sia konscie lama <tld/>o
        <fnt>
          <bib>Ret</bib>
          <lok>Aldono</lok>
        </fnt>;
      </ekz>
      <ekz>
        la subregisto, de tempo jam longa, preparis sian <tld/>on
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        al li ne mankas <tld/>o kontra&ubreve; ofendo;
        <fnt><bib>PrV</bib></fnt>
      </ekz>
      <ekz>
        senfara plendo ne estas <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="apolog1.0o">apologio</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_a;&c_r;&c_o;&c_n;&c_a;</trd>,
    <trd>&c_a;&c_h;&c_o;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>obhajoba</trd>,
    <trd>obrana</trd>,
    <trd>ochrana</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Verteidigung</trd>,
    <trd>Abwehr</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>defense</trd>,
    <trd>defence</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;fense</trd>
  <trd lng="he">&he;&gimel;&nun;&he;</trd>
  <trdgrp lng="ja">
    <trd>&#x9632;&#x5FA1;  <pr>&#x307C;&#x3046;&#x304E;&#x3087;</pr></trd>,
    <trd>&#x9632;&#x885B;  <pr>&#x307C;&#x3046;&#x3048;&#x3044;</pr></trd>,
    <trd>&#x5F01;&#x8B77;  <pr>&#x3079;&#x3093;&#x3054;</pr></trd>
  </trdgrp>
  <trd lng="nl">verdediging</trd>
  <trd lng="pl">obrona</trd>
  <trdgrp lng="sk">
    <trd>obhajoba</trd>,
    <trd>obrana</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_h;&c_i;&c_s;&c_t;</trd>,
    <trd>&c_o;&c_b;&c_o;&c_r;&c_o;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9632;&#x5FA1; <pr>f&#xE1;ngy&#xF9;</pr></trd>,
    <trd>&#x4FDD;&#x536B; <pr>b&#x1CE;ow&#xE8;i</pr></trd>
  </trdgrp>
</drv>

