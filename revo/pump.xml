<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pump</rad>/i
</kap>
<drv mrk="pump.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Altiri a&ubreve; repu&scirc;i flua&jcirc;on a&ubreve; gason per 
<tld/>ilo:
      <ekz>
        <tld/>i akvon por &gcirc;ardeno, aeron en bicikletan 
pne&ubreve;matikon.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_m;&c_p;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">pumpen</trd>
  <trd lng="en">pump</trd>
  <trdgrp lng="es">
    <trd>bombear</trd>
    <trd>comprimir</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szivatty&uacute;z</trd>,
    <trd>pump&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">pompen</trd>
  <trd lng="ru">&c_k;&c_a;&c_ch;&c_a;&c_t;&c_mol; <klr 
tip="amb">(&c_n;&c_a;&c_s;&c_o;&c_s;&c_o;&c_m;)</klr></trd>
</drv>

<drv mrk="pump.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Speciala aparato, uzanta maldensigon de la aero por levi 
flua&jcirc;on en
      tubo.
    </dif>
  </snc>
  <snc>
    <dif>
      Analoga aparato, uzata por plidensigi a&ubreve; plimaldensigi 
gason en ujo.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_o;&c_m;&c_p;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_o;&c_s;</trd>,
    <trd>&c_k;&c_a;&c_m;&c_p;&c_r;&c_e;&c_s;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="de">Pumpe</trd>
  <trdgrp lng="es">
    <trd>bomba</trd>
    <trd>compresor</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szivatty&uacute;</trd>,
    <trd>pumpa</trd>
  </trdgrp>
  <trd lng="nl">pomp</trd>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_o;&c_s;</trd>
</drv>

<drv mrk="pump.aer0ilo">
  <kap>aer<tld/>ilo</kap>
  <snc>
    <dif>
      Ma&scirc;ino a&ubreve; ilo por <tld/>i aeron.
    </dif>
  </snc>
  <trd lng="en">air <ind>pump</ind></trd>
  <trd lng="es">compresor</trd>
  <trd lng="hu">l&eacute;gszivatty&uacute;</trd>
  <trd lng="nl">persluchtmachine</trd>
  <trd lng="ru">&c_v;&c_o;&c_z;&c_d;&c_u;&c_sh;&c_n;&c_y;&c_j; 
&c_n;&c_a;&c_s;&c_o;&c_s;</trd>
</drv>

<drv mrk="pump.Aer0ilo">
  <kap>Aer<tld/>ilo</kap>
  <mlg>Ant</mlg>
  <snc>
    <dif>
      Nord-hemisfera <ref tip="super" 
cel="stel.0figuro">stelfiguro</ref> (<trd lng="la">Antlia</trd>).
    </dif>
  </snc>
  <trd lng="de">Luftpumpe</trd>
  <trd lng="en">Antlia</trd>
  <trd lng="es">M&aacute;quina Neum&aacute;tica</trd>
</drv>

</art>
