<kap>
  <rad>enklav</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="enklav.0o">
  <kap><tld/>o</kap>
  <snc mrk="enklav.0o.POL">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Landeto &ccirc;irka&ubreve;ata de fremda medio kaj disa de
      &ccirc;eflando:
      <ekz>
        Ceuta, hispana <tld/>o en Maroko, kie tricent registritaj
        azilpetantoj dormas surstrate
        <fnt>
        <aut>A. Morice, trad. J.-M. Cash</aut>,
        <vrk><url ref="&Mondediplo;article887.html">Kiel
        E&ubreve;ropo for&scirc;ovas la rifu&gcirc;intojn</url></vrk>,
        <lok>le Monde diplomatique, 2004-03</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        konservativa <tld/>o meze de maro da protestoj kaj manifestacioj
        pri libero kaj demokratio
        <fnt>
        <aut>&Gcirc;angalo</aut>,
        <vrk><url ref="http://gxangalo.com/modules/news/article.php?storyid=1374">
        Pli ol 100 iranaj deputitoj demisiis...</url></vrk>,
        <lok>2004-02-01</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="insul.0o.GEOG">insulo</ref>,
      <ref cel="oaz.0o.GEOG">oazo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_k;&c_l;&c_ja;&c_w;</trd>
  <trd lng="cs">enkl&aacute;va</trd>
  <trd lng="en">enclave</trd>
  <trd lng="fr">enclave</trd>
  <trd lng="id">enklave</trd>
  <trd lng="pl">enklawa</trd>
  <trdgrp lng="pt">
    <trd>encrave</trd>,
    <trd>enclave</trd>,    
    <trd>encravo</trd>
  </trdgrp>
  <trd lng="sk">enkl&aacute;va</trd>
  <trdgrp lng="uk">
    <trd>&#x430;&#x43D;&#x43A;&#x43B;&#x430;&#x432;</trd>,
    <trd>&#x435;&#x43D;&#x43A;&#x43B;&#x430;&#x432;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x88AB;&#x5305;&#x56F4;&#x7684;&#x9886;&#x571F; <pr>b&#xE8;ib&#x101;ow&#xE9;idel&#x1D0;ngt&#x1D4;</pr></trd>,
    <trd>&#x5167;&#x98DB;&#x5730; <pr>n&#xE8;if&#x113;ide</pr></trd>
  </trdgrp>
</drv>
</art>

