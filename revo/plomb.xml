<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <rad>plomb</rad>/o
   <fnt>Z</fnt>
</kap>
<drv mrk="plomb.0o">
   <kap><tld/>o</kap>
   <snc>
     <dif>
       Metala cemento por kurac&scirc;topi difektitajn dentojn.
     </dif>
   </snc>
  <trd lng="be">&c_p;&c_l;&c_o;&c_m;&c_b;&c_a;</trd>
  <trdgrp lng="de">
     <trd>Plombe</trd>,
     <trd>F&uuml;llung</trd>
   </trdgrp>
  <trdgrp lng="fr">
     <trd>ciment<klr tip="ind"> (dent.)</klr></trd>,
     <trd>amalgame<klr tip="ind"> (dent.)</klr></trd>
   </trdgrp>
  <trd lng="hu">t&ouml;m&eacute;s<klr tip="ind"> (fogban)</klr></trd>
  <trdgrp lng="nl">
     <trd>plombeersel</trd>,
     <trd>vulling</trd>,
     <trd>tandvulling</trd>
   </trdgrp>
  <trd lng="pt">cimento met&aacute;lico <klr>(para dentes)</klr></trd>
  <trd lng="ru">&c_p;&c_l;&c_o;&c_m;&c_b;&c_a;</trd>
  <trd lng="sk">plomba</trd>
</drv>

<drv mrk="plomb.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc>
     <dif>
       &Scirc;topi denton per <tld/>o.
     </dif>
   </snc>
  <trdgrp lng="be">
     <trd>&c_p;&c_l;&c_a;&c_m;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
 
<trd>&c_z;&c_a;&c_p;&c_l;&c_a;&c_m;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
   </trdgrp>
  <trdgrp lng="de">
     <trd>plombieren</trd>,
     <trd>f&uuml;llen</trd>
   </trdgrp>
  <trd lng="fr">plomber</trd>
  <trd lng="hu">t&ouml;m<klr tip="ind"> (fogat)</klr></trd>
  <trdgrp lng="nl">
     <trd>vullen<klr tip="ind"> (tanden)</klr></trd>,
     <trd>plomberen</trd>
   </trdgrp>
  <trdgrp lng="ru">
 
<trd>&c_p;&c_l;&c_o;&c_m;&c_b;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
 
<trd>&c_z;&c_a;&c_p;&c_l;&c_o;&c_m;&c_b;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>plombovať</trd>,
    <trd>zaplombovať</trd>
  </trdgrp>
</drv>
</art>
