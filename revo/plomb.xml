<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: plomb.xml,v 1.21 2024/02/17 07:52:16 revo Exp $">
<kap>
   <rad>plomb</rad>/o
   <fnt>Z</fnt>
</kap>
<drv mrk="plomb.0o">
  <kap><tld/>o</kap>
  <snc mrk="plomb.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
       Metala cemento por kurac&scirc;topi difektitajn dentojn:
      <ekz>
        oni volas malpermesi la uzadon de amalgamo en la EU kiel <tld/>on
        (dento&scirc;topa&jcirc;on), kvankam fakspertuloj asertas ties nedan&gcirc;erecon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=35620">Amalgamo</url></lok>
        </fnt>.
      </ekz>
    </dif> 
    <trd lng="be">&c_p;&c_l;&c_o;&c_m;&c_b;&c_a;</trd>
    <trd lng="cs">plomba</trd>
    <trdgrp lng="de">
       <trd>Plombe</trd>,
       <trd>F&uuml;llung</trd>
     </trdgrp>
    <trdgrp lng="en">
      <trd>dental filling</trd>,
      <trd>dental cement</trd>
    </trdgrp>
    <trdgrp lng="es">
       <trd>amalgama<klr tip="ind"> (dent.)</klr></trd>
     </trdgrp>
    <trdgrp lng="fr">
       <trd>ciment<klr tip="ind"> (dent.)</klr></trd>,
       <trd>amalgame<klr tip="ind"> (dent.)</klr></trd>
     </trdgrp>
    <trd lng="hu">t&ouml;m&eacute;s<klr tip="ind"> (fogban)</klr></trd>
    <trdgrp lng="nl">
       <trd>plombeersel</trd>,
       <trd>vulling</trd>,
       <trd>tandvulling</trd>
     </trdgrp>
     <trd lng="pl">plomba</trd>
    <trd lng="pt">cimento met&aacute;lico <klr>(para dentes)</klr></trd>
    <trd lng="ro">plomb&abreve;</trd>
    <trd lng="ru">&c_p;&c_l;&c_o;&c_m;&c_b;&c_a;</trd>
    <trd lng="sk">plomba</trd>
  </snc>
  <snc mrk="plomb.0o.sigelo">
    <dif>
       Sekuriga <ref tip="super" cel="sigel.0o.TIP_fermi">sigelo</ref> por ujo, 
       ekz. &ccirc;ambro post okazinta krimo, kontenero ks.
    </dif>
    <trd lng="en">seal</trd>
    <trd lng="pl">plomba</trd>
    <trd lng="ro">sigiliu</trd>
    <trd lng="zh">&#x5370;&#x7AE0; <pr>y&#xEC;nzh&#x101;ng</pr></trd>
</snc>
  <trdgrp lng="ja">
    <trd>&#x5145;&#x586B;&#x5264;  <pr>&#x3058;&#x3085;&#x3046;&#x3066;&#x3093;&#x3056;&#x3044;</pr></trd>,
    <trd>&#x6B6F;&#x79D1;&#x7528;&#x30BB;&#x30E1;&#x30F3;&#x30C8;  <pr>&#x3057;&#x304B;&#x3088;&#x3046;&#x305B;&#x3081;&#x3093;&#x3068;</pr></trd>,
    <trd>&#x30A2;&#x30DE;&#x30EB;&#x30AC;&#x30E0;</trd>
  </trdgrp>
  <trd lng="uk">&c_p;&c_l;&c_o;&c_m;&c_b;&c_a; (&c_z;&c_u;&c_b;&c_a;)</trd>
</drv>

<drv mrk="plomb.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="plomb.0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
       &Scirc;topi denton per <tld/>o.
    </dif>
    <trdgrp lng="be">
       <trd>&c_p;&c_l;&c_a;&c_m;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
       <trd>&c_z;&c_a;&c_p;&c_l;&c_a;&c_m;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="cs">plombovat</trd>
    <trdgrp lng="de">
       <trd>plombieren</trd>,
       <trd>f&uuml;llen</trd>
     </trdgrp>
    <trd lng="es">empastar</trd>
    <trd lng="fr">plomber</trd>
    <trd lng="hu">t&ouml;m<klr tip="ind"> (fogat)</klr></trd>
    <trdgrp lng="nl">
       <trd>vullen<klr tip="ind"> (tanden)</klr></trd>,
       <trd>plomberen</trd>
     </trdgrp>
     <trd lng="pl">plombowa&cacute;</trd>
     <trd lng="ro">sigila</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_o;&c_m;&c_b;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_p;&c_l;&c_o;&c_m;&c_b;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>plombova&tcaron;</trd>,
      <trd>zaplombova&tcaron;</trd>
    </trdgrp>
  </snc>
  <snc mrk="plomb.0i.sigeli">
    <dif>
      Provizi per sekuriga sigelo:
      <ekz>
        dum la Unua mondmilito <nom>Lenin</nom> kaj sia akompanantaro voja&gcirc;is per <tld/>ita
        trajnvagono tra Germanio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=479818">Fritz Platten</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>put a filling in a tooth</trd>,
    <trd>affix a seal</trd>
  </trdgrp>
  <trd lng="ja">&#x5145;&#x586B;&#x3059;&#x308B;  <pr>&#x3058;&#x3085;&#x3046;&#x3066;&#x3093;&#x3059;&#x308B;</pr></trd>
  <trd lng="uk">&c_p;&c_l;&c_o;&c_m;&c_b;&c_u;&c_v;&c_a;&c_t;&c_i; (&c_z;&c_u;&c_b;&c_i;)</trd>
  <trdgrp lng="zh">
    <trd>&#x76D6;&#x7AE0; <pr>g&#xE0;izh&#x101;ng</pr></trd>,
    <trd>&#x84CB;&#x7AE0; <pr>g&#xE0;izh&#x101;ng</pr></trd>
  </trdgrp>
</drv>
</art>
