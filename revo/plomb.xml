<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>plomb</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="plomb.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Metala cemento por kurac&scirc;topi difektitajn dentojn.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Plombe</trd>,
    <trd>F&uuml;llung</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>plombeersel</trd>,
    <trd>vulling</trd>,
    <trd>tandvulling</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_l;&c_o;&c_m;&c_b;&c_a;</trd>
</drv>

<drv mrk="plomb.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;topi denton per <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>plombieren</trd>,
    <trd>f&uuml;llen</trd>
  </trdgrp>
  <trd lng="nl">plomberen</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_o;&c_m;&c_b;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_l;&c_o;&c_m;&c_b;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
