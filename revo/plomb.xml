<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: plomb.xml,v 1.23 2024/12/10 12:54:39 revo Exp $">
<kap>
   <rad>plomb</rad>/o
   <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="plomb.0o">
  <kap><tld/>o</kap>
  <snc mrk="plomb.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Metala cemento por kurac&scirc;topi difektitajn dentojn:
      <ekz>
        oni volas malpermesi la uzadon de amalgamo en la EU kiel <tld/>on
        (dento&scirc;topa&jcirc;on), kvankam fakspertuloj asertas ties nedan&gcirc;erecon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=35620">Amalgamo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_l;&c_o;&c_m;&c_b;&c_a;</trd>
    <trd lng="cs">plomba</trd>
    <trdgrp lng="de">
       <trd>Plombe</trd>,
       <trd>F&uuml;llung</trd>
     </trdgrp>
    <trdgrp lng="en">
      <trd>dental filling</trd>,
      <trd>dental cement</trd>
    </trdgrp>
    <trdgrp lng="es">
       <trd>amalgama<klr tip="ind"> (dent.)</klr></trd>
     </trdgrp>
    <trdgrp lng="fr">
       <trd>ciment<klr tip="ind"> (dent.)</klr></trd>,
       <trd>amalgame<klr tip="ind"> (dent.)</klr></trd>
     </trdgrp>
    <trd lng="hu">t&ouml;m&eacute;s<klr tip="ind"> (fogban)</klr></trd>
    <trdgrp lng="nl">
       <trd>plombeersel</trd>,
       <trd>vulling</trd>,
       <trd>tandvulling</trd>
     </trdgrp>
     <trd lng="pl">plomba</trd>
    <trd lng="pt">cimento met&aacute;lico <klr>(para dentes)</klr></trd>
    <trd lng="ro">plomb&abreve;</trd>
    <trd lng="ru">&c_p;&c_l;&c_o;&c_m;&c_b;&c_a;</trd>
    <trd lng="sk">plomba</trd>
  </snc>
  <snc mrk="plomb.0o.sigelo">
    <dif>
       Sekuriga <ref tip="super" cel="sigel.0o.TIP_fermi">sigelo</ref> por ujo,
       ekz. &ccirc;ambro post okazinta krimo, kontenero ks.
    </dif>
    <trd lng="be">&c_p;&c_l;&c_o;&c_m;&c_b;&c_a;</trd>
    <trd lng="en">seal</trd>
    <trd lng="pl">plomba</trd>
    <trd lng="ro">sigiliu</trd>
    <trd lng="zh">&#21360;&#31456; <pr>y&igrave;nzh&amacron;ng</pr></trd>
  </snc>
  <snc mrk="plomb.0o.plumbo">
    <uzo tip="stl">EVI</uzo>
    <dif>
      Plumbo:
      <ekz>
        &ccirc;iu tago estus por mi nova &scirc;ar&gcirc;o, pezanta sur mia brusto kiel <tld/>o
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pafo eksplodas, boretante per <tld/>eroj nur la bran&ccirc;ojn de l&#8217; apuda arbeto
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Kvara</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    Etimologie &leftquot;<tld/>o&rightquot; rilatas al <ref tip="hom" cel="plumb.0o">plumbo</ref>, kiun oni iam uzis por sigelado
    <aut>Wolfram Diestel</aut>.
  </rim>
  <trdgrp lng="ja">
    <trd>&#20805;&#22635;&#21092;  <pr>&#12376;&#12421;&#12358;&#12390;&#12435;&#12374;&#12356;</pr></trd>,
    <trd>&#27503;&#31185;&#29992;&#12475;&#12513;&#12531;&#12488;  <pr>&#12375;&#12363;&#12424;&#12358;&#12379;&#12417;&#12435;&#12392;</pr></trd>,
    <trd>&#12450;&#12510;&#12523;&#12460;&#12512;</trd>
  </trdgrp>
  <trd lng="uk">&c_p;&c_l;&c_o;&c_m;&c_b;&c_a; (&c_z;&c_u;&c_b;&c_a;)</trd>
</drv>

<drv mrk="plomb.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="plomb.0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
       <ref tip="super" cel="sxtop.0i.fermi">&Scirc;topi</ref> denton per <tld/>o:
       <ekz>
        la tekniko de la <tld/>ado kaj de la eltiro tamen ne povis evolui al tiom
        mistika sor&ccirc;o, ke mi ne povus &gcirc;in ellerni
        <fnt>
          <bib>VaK</bib>
          <lok>Sesa &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
       <trd>&c_p;&c_l;&c_a;&c_m;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
       <trd>&c_z;&c_a;&c_p;&c_l;&c_a;&c_m;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="cs">plombovat</trd>
    <trdgrp lng="de">
       <trd>plombieren</trd>,
       <trd>f&uuml;llen</trd>
     </trdgrp>
    <trd lng="es">empastar</trd>
    <trd lng="fr">plomber</trd>
    <trd lng="hu">t&ouml;m<klr tip="ind"> (fogat)</klr></trd>
    <trdgrp lng="nl">
       <trd>vullen<klr tip="ind"> (tanden)</klr></trd>,
       <trd>plomberen</trd>
     </trdgrp>
     <trd lng="pl">plombowa&cacute;</trd>
     <trd lng="ro">sigila</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_o;&c_m;&c_b;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_p;&c_l;&c_o;&c_m;&c_b;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>plombova&tcaron;</trd>,
      <trd>zaplombova&tcaron;</trd>
    </trdgrp>
  </snc>
  <snc mrk="plomb.0i.sigeli">
    <dif>
      Provizi per sekuriga sigelo:
      <ekz>
        dum la Unua mondmilito <nom>Lenin</nom> kaj sia akompanantaro voja&gcirc;is per <tld/>ita
        trajnvagono tra Germanio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=479818">Fritz Platten</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
       <trd>&c_p;&c_l;&c_a;&c_m;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
       <trd>&c_a;&c_p;&c_l;&c_a;&c_m;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
  </snc>
   <trdgrp lng="en">
    <trd>put a filling in a tooth</trd>,
    <trd>affix a seal</trd>
  </trdgrp>
  <trd lng="ja">&#20805;&#22635;&#12377;&#12427;  <pr>&#12376;&#12421;&#12358;&#12390;&#12435;&#12377;&#12427;</pr></trd>
  <trd lng="uk">&c_p;&c_l;&c_o;&c_m;&c_b;&c_u;&c_v;&c_a;&c_t;&c_i; (&c_z;&c_u;&c_b;&c_i;)</trd>
  <trdgrp lng="zh">
    <trd>&#30422;&#31456; <pr>g&agrave;izh&amacron;ng</pr></trd>,
    <trd>&#33995;&#31456; <pr>g&agrave;izh&amacron;ng</pr></trd>
  </trdgrp>
</drv>
</art>
