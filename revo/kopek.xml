<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kopek</rad>/o
</kap>
<drv mrk="kopek.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Rusa tre malgranda monero, centono de
      <ref tip="malprt" cel="rubl.0o">rublo</ref>:
      <ekz>
        <ind>unu<tld/>a kandelo bruligis Moskvon</ind><fnt>Z</fnt>
        <klr>(eta ka&ubreve;zo, granda efiko)</klr>.
        <trd lng="ru">
        &c_o;&c_t; 
        &c_k;&c_o;&c_p;&c_je;&c_je;&c_ch;&c_n;&c_o;&c_j; 
        &c_s;&c_v;&c_je;&c_ch;&c_i; 
        &c_M;&c_o;&c_s;&c_k;&c_v;&c_a; 
        &c_s;&c_g;&c_o;&c_r;&c_je;&c_l;&c_a;
        </trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kul.0o">kulo</ref>.
    </refgrp>
    <refgrp tip="sin">
      <ref cel="grosx.0o">gro&scirc;o</ref>,
      <ref cel="pfenig.0o">pfenigo</ref>,
      <ref cel="centim.0o">centimo</ref>,
      <ref cel="cend.0o">cendo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_p;&c_je;&c_j;&c_k;&c_a;</trd>
  <trd lng="de">Kopeke</trd>
  <trd lng="en">copeck</trd>
  <trdgrp lng="hu">
    <trd>kopek</trd>,
    <trd>kopejka</trd>
  </trdgrp>
  <trd lng="pl">kopiejka</trd>
  <trd lng="pt">copeque</trd>
  <trd lng="ru">&c_k;&c_o;&c_p;&c_je;&c_j;&c_k;&c_a;</trd>
</drv>
</art>
