<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kopek.xml,v 1.20 2023/12/03 09:58:58 revo Exp $">
<kap>
  <ofc>*</ofc><ofc>1</ofc><ofc>7</ofc>
  <rad>kopek</rad>/o
</kap>
<drv mrk="kopek.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="kopek.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Rusa tre malgranda <ref tip="lst" lst="voko:monunuoj" cel="mon.0ero">monero</ref>,
      centono de <ref tip="malprt" cel="rubl.0o">rublo</ref>:
      <ekz>
        centimo, pfenigo kaj kopeko estas moneroj. 
        <fnt><bib>F</bib>, <lok>&FE; 41</lok></fnt>.
      </ekz>
      <ekz>
        jen dudek <tld/>oj, prenu veturigiston kaj veturu hejmen
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la kosto de &ccirc;iu kajero estos 25 <tld/>oj
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; unu <tld/>on ne volas pagi
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia vivo <klr>[&#x2026;]</klr> ne valoras por mi e&ccirc; <tld/>on
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>unu<tld/>a kandelo bruligis Moskvon</ind>
        <fnt>
          <aut>Sabine Fiedler</aut>
          <vrk><url ref="https://hrcak.srce.hr/file/204215">Esperanto Phraseology</url></vrk>
        </fnt>
        <klr>(eta ka&ubreve;zo, granda efiko, kp <ref cel="kul.0o">kulo</ref>)</klr>.
        <trd lng="ru">
        &c_o;&c_t; 
        &c_k;&c_o;&c_p;&c_je;&c_je;&c_ch;&c_n;&c_o;&c_j; 
        &c_s;&c_v;&c_je;&c_ch;&c_i; 
        &c_M;&c_o;&c_s;&c_k;&c_v;&c_a; 
        &c_s;&c_g;&c_o;&c_r;&c_je;&c_l;&c_a;
        </trd>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="grosx.0o">gro&scirc;o</ref>,
      <ref cel="pfenig.0o">pfenigo</ref>,
      <ref cel="centim.0o">centimo</ref>,
      <ref cel="cend.0o">cendo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_p;&c_je;&c_j;&c_k;&c_a;</trd>
  <trd lng="de">Kopeke</trd>
  <trdgrp lng="en">
    <trd>copeck</trd>,
    <trd>kopeck</trd>,
    <trd>kopek</trd>
  </trdgrp>
  <trd lng="fr">kopeck</trd>
  <trdgrp lng="hu">
    <trd>kopek</trd>,
    <trd>kopejka</trd>
  </trdgrp>
  <trd lng="ja">&#x30B3;&#x30DA;&#x30A4;&#x30AB;</trd>
  <trdgrp lng="nl">
    <trd>kopek</trd>,
    <trd>kopeke</trd>
  </trdgrp>
  <trd lng="pl">kopiejka</trd>
  <trd lng="pt">copeque</trd>
  <trd lng="ru">&c_k;&c_o;&c_p;&c_je;&c_j;&c_k;&c_a;</trd>
  <trd lng="zh">&#x6208;&#x6BD4; <pr>g&#x113;b&#x1D0;</pr></trd>
</drv>
</art>
