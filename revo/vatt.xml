<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>Vatt</rad>/o
</kap>
<drv mrk="vatt.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <uzo tip="stl">EVI</uzo>
  <ref tip="dif" cel="vat1.0o">Vato.</ref>
</drv>
<drv mrk="vatt.v0o">
  <kap><tld lit="v"/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <uzo tip="stl">EVI</uzo>
  <ref tip="dif" cel="vat1.v0o">vato.</ref>
</drv>
<drv mrk="vatt.v0horo">
  <kap><tld lit="v"/>horo, <var><kap><tld lit="v"/>ohoro</kap></var></kap>
  <uzo tip="fak">FIZ</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <uzo tip="stl">EVI</uzo>
  <ref tip="dif" cel="vat1.v0horo">vathoro.</ref>
</drv>
</art>

