<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>Vatt</rad>/o
</kap>
<drv mrk="vatt.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Anglalingve: James <trd lng="en">Watt</trd>, 1736-1819. Brita fizikisto.
    </dif>
  </snc>
  <trd lng="fr">Watt</trd>
  <trd lng="ru">&c_V;&c_a;&c_t;&c_t;</trd>
</drv>
<drv mrk="vatt.v0o">
  <kap><tld lit="v"/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de
      <ref cel="pov.0o.FIZ">povo</ref> en la Internacia Sistemo de
      unuoj; simb. W:
      <ekz>
        unu <tld lit="v"/>o egalas al la povo, &ccirc;e kiu laboro je unu
        <ref cel="jxul.jx0o">&jcirc;ulo</ref> estas produktata en
        unu <ref cel="sekund.0o.tempo">sekundo</ref>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">watt<klr tip="ind"> (unit&eacute; de mesure)</klr></trd>
  <trd lng="ru">&c_v;&c_a;&c_t;&c_t;</trd>
</drv>
<drv mrk="vatt.v0horo">
  <kap><tld lit="v"/>horo</kap>
  <uzo tip="fak">FIZ</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Komunuza <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de
      <ref cel="energi.0o.FIZ">energio</ref>; simb. Wh:
      <ekz>
        unu <tld lit="v"/>horo egalas al la laboro produktata de povo
        je unu <tld lit="v"/>o dum unu horo, alidire 3600 &jcirc;uloj.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">watt-heure</trd>
  <trd lng="ru">&c_v;&c_a;&c_t;&c_t;-&c_ch;&c_a;&c_s;</trd>
</drv>
</art>

<!--
$Log$
