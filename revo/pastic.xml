<art mrk="$Id: pastic.xml,v 1.21 2025/04/04 18:41:26 revo Exp $">
<kap>
  <rad>pasti&ccirc;</rad>/o
<fnt><bib>PVEF</bib></fnt></kap>
<drv mrk="pastic.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="verk.0o.propra">Verko</ref>,
      en kiu oni &scirc;erce imitas la manieron kaj stilon de fama
      artisto:
      <ekz>
        humura kolekto de parodioj kaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2013/010593.php">&Ccirc;e l' koro de E&ubreve;ropo ...</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la estonteco estas jam nula &ndash; <tld/>o de la FIDESZ-slogano &leftquot;la estonteco estas
        jam nuna&#8221;, la estonteco jam demenci&gcirc;is &ndash; <tld/>o de la FIDESZ-slogano &leftquot;la
        estonteco jam komenci&gcirc;is&#8221;
        <fnt>
          <bib>Monato</bib>
          <aut>Istv&aacute;n ERTL</aut>
          <vrk><url ref="&Monato;/2003/007869.html">Vo&ccirc;o de vo&ccirc;donantoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="parodi.0o">parodio</ref>,
      <ref cel="plagia.0o">plagiato</ref>
    </refgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>stilimito</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_o;&c_d;&c_y;&c_ja;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_a;&c_d;&c_y;&c_j;&c_n;&c_y; &c_t;&c_v;&c_o;&c_r;</trd>
  </trdgrp>
  <trd lng="ca">pastitx</trd>
  <trdgrp lng="de">
    <trd>Pasticcio</trd>,
    <trd>Nachahmung</trd>,
    <trd>Verhohnepipelung</trd>,
    <trd>Verhunzung</trd>
  </trdgrp>
  <trd lng="en">pastiche</trd>
  <trdgrp lng="fr">
    <trd>pastiche</trd>
  </trdgrp>
  <trd lng="hu">pastiche<klr tip="amb"> (ut&aacute;nzat)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#12497;&#12473;&#12486;&#12451;&#12540;&#12471;&#12517;</trd>,
    <trd>&#27169;&#20515;&#20316;&#21697;  <pr>&#12418;&#12411;&#12358;&#12373;&#12367;&#12402;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">pastiche</trd>
  <trd lng="pl">pastisz</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_r;&c_a;&c_zh;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_o;&c_d;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_a;&c_r;&c_o;&c_d;&c_ib;&c_ja;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_l;&c_ib;&c_d;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#25311;&#20316; <pr>n&#464;zu&#242;</pr></trd>,
    <trd>&#25836;&#20316; <pr>n&#464;zu&#242;</pr></trd>,
    <trd>&#25311;&#21476;&#20043;&#20316; <pr>n&#464;g&#468;zh&#299;zu&#242;</pr></trd>,
    <trd>&#25836;&#21476;&#20043;&#20316; <pr>n&#464;g&#468;zh&#299;zu&#242;</pr></trd>
  </trdgrp>
</drv>
