<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>9</ofc><rad>kanguru</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kangur.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="marsup.0o"
      lst="voko:marsupiuloj">Marsupiulo</ref>
      movanta sin saltante per la malanta&ubreve;aj kruroj kaj kies ino
      havas ventran po&scirc;on por gardi la idojn;
      iu el la familio <ref tip="malprt"
      cel="makrop1.0oj">makropedoj</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_je;&c_n;&c_g;&c_u;&c_r;&c_u;</trd>
  <trd lng="ca">cangur</trd>
  <trd lng="cs">klokan</trd>
  <trd lng="de">K&auml;nguru</trd>
  <trd lng="en">kangaroo</trd>
  <trd lng="es">canguro</trd>
  <trd lng="fr">kangourou</trd>
  <trd lng="hu">kenguru</trd>
  <trd lng="id">kanguru</trd>
  <trd lng="nl">kangoeroe</trd>
  <trd lng="pl">kangur</trd>
  <trd lng="ru">&c_k;&c_je;&c_n;&c_g;&c_u;&c_r;&c_u;</trd>
  <trdgrp lng="sk">
    <trd>kengura</trd>,
    <trd>klokan</trd>
  </trdgrp>
</drv>
</art>
