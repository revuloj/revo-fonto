<kap>
  <ofc>1</ofc>
  <rad>ambici</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ambici.0o">
  <kap><tld/>o</kap>
  <snc mrk="ambici.0o.PSI">
    <dif>
      Gloramo, honoramo:
      <ekz>
        ekzistas la&ubreve;dindaj <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <snc mrk="ambici.0o.KOMUNE">
    <dif>
      Afero aspirata, dezirata, celata pro <tld/>a sento:
      <ekz>
        politikaj <tld/>oj de subridantaj mistifikistoj
        <fnt>
          <aut>K. Kalocsay</aut>,
          <vrk>Mia penso</vrk>,
          <lok>Norda Prismo, 1957:4, p. 182a-185a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_b;&c_ib;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>ambice</trd>,
    <trd>aspirace</trd>,
    <trd>cti&zcaron;&aacute;dost</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Ehrgeiz</trd>,
     <trd>Streben</trd>,
     <trd>Ambition</trd>
  </trdgrp>
  <trd lng="en">ambition</trd>
  <trd lng="es">ambici&oacute;n</trd>
  <trd lng="fr">ambition</trd>
  <trd lng="he">&#x5E9;&#x5C1;&#x5B0;&#x5D0;&#x5B7;&#x5E4;&#x5EA;&#x5B8;&#x5E0;&#x5D5;&#x5BC;&#x5EA;</trd>
  <trdgrp lng="nl">
    <trd>ambitie</trd>,
    <trd>eerzucht</trd>
  </trdgrp>
  <trd lng="pl">ambicja</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_m;&c_b;&c_i;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_ch;&c_je;&c_s;&c_t;&c_o;&c_l;&c_ju;&c_b;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>amb&iacute;cia</trd>,
    <trd>a&scaron;pir&aacute;cia</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x430;&#x43C;&#x431;&#x456;&#x446;&#x456;&#x44F;</trd>,
    <trd>&#x441;&#x430;&#x43C;&#x43E;&#x43B;&#x44E;&#x431;&#x441;&#x442;&#x432;&#x43E;</trd>,
    <trd>&#x447;&#x435;&#x441;&#x442;&#x43E;&#x43B;&#x44E;&#x431;&#x441;&#x442;&#x432;&#x43E;</trd>,
    <trd>&#x432;&#x43B;&#x430;&#x441;&#x442;&#x43E;&#x43B;&#x44E;&#x431;&#x441;&#x442;&#x432;&#x43E;</trd>,
    <trd>&#x433;&#x43E;&#x43D;&#x43E;&#x440;</trd>,
    <trd>&#x447;&#x432;&#x430;&#x43D;&#x441;&#x442;&#x432;&#x43E;</trd>,
    <trd>&#x43F;&#x438;&#x445;&#x430;</trd>
  </trdgrp>
</drv>

<drv mrk="ambici.0a">
  <kap><tld/>a</kap>
  <snc mrk="ambici.0a.PSI">
    <dif>
      Havanta <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_b;&c_ib;&c_c;&c_y;&c_j;&c_n;&c_y;</trd>
  <trd lng="he">&#x5E9;&#x5C1;&#x5B0;&#x5D0;&#x5B7;&#x5E4;&#x5EA;&#x5B8;&#x5E0;&#x5D9;</trd>
  <trd lng="ms">cita-cita</trd>
  <trdgrp lng="cs">
    <trd>ambici&oacute;zn&iacute;</trd>,
    <trd>n&aacute;ro&ccaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>ehrgeizig</trd>,
     <trd>strebsam</trd>,
     <trd>ambitioniert</trd>
  </trdgrp>
  <trd lng="en">ambitious</trd>
  <trd lng="es">ambicioso</trd>
  <trd lng="fr">ambitieux</trd>
  <trd lng="nl">eerzuchtig</trd>
  <trd lng="pl">ambitny</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_m;&c_b;&c_i;&c_c;&c_i;&c_o;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_ch;&c_je;&c_s;&c_t;&c_o;&c_l;&c_ju;&c_b;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ambici&oacute;zny</trd>,
    <trd>ambici&oacute;zny</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x447;&#x435;&#x441;&#x442;&#x43E;&#x43B;&#x44E;&#x431;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x432;&#x43B;&#x430;&#x441;&#x442;&#x43E;&#x43B;&#x44E;&#x431;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x430;&#x43C;&#x431;&#x456;&#x442;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x430;&#x43C;&#x431;&#x456;&#x446;&#x456;&#x439;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x43F;&#x438;&#x445;&#x430;&#x442;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="ambici.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="ambici.0i.PSI">
    <dif>
      Esti <tld/>a pri:
      <ekz>
        mi ne <tld/>as tian gloron, al tia gloro.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_c;&c_mol; <ind>&c_a;&c_m;&c_b;&c_ib;&c_c;&c_y;&c_ju;</ind></trd>
  <trdgrp lng="cs">
    <trd>cti&zcaron;&aacute;dostiv&ecaron; usilovat</trd>,
    <trd>dychtit</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Ehrgeiz zeigen</trd>,
     <trd>ehrgeizig sein</trd>,
     <trd>streben</trd>
  </trdgrp>
  <trd lng="en">have the ambition to</trd>
  <trdgrp lng="fr">
    <trd>avoir l'<ind>ambition</ind> (de)</trd>,
    <trd>ambitionner (de)</trd>,
    <trd>aspirer (&agrave;)</trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5E9;&#x5D0;&#x5D5;&#x5E3;</trd>
  <trd lng="nl">ambi&euml;ren</trd>
  <trdgrp lng="pl">
  <trd>by&cacute; <ind>ambitnym</ind></trd>,
  <trd>mie&cacute; <ind>ambicj&eogonek;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ma&tcaron; <ind>amb&iacute;ciu</ind></trd>,
    <trd>ma&tcaron; amb&iacute;ciu</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x434;&#x43E;&#x43C;&#x430;&#x433;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>,
    <trd>&#x434;&#x43E;&#x431;&#x438;&#x432;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>,
    <trd>&#x43F;&#x440;&#x430;&#x433;&#x43D;&#x443;&#x442;&#x438;</trd>
  </trdgrp>
</drv>
