</dif>
</snc>
<trd lng="fr">ambitieux</trd>
<trdgrp lng="ru">
  <trd>&#1072;&#1084;&#1073;&#1080;&#1094;&#1080;&#1086;&#1079;&#1085;&#1099;&#1081;</trd>,
