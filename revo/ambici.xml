<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>ambici</rad>/o
</kap>
<drv mrk="ambici.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Gloramo, honoramo:
      <ekz>
        ekzistas la&ubreve;dindaj <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Ehrgeiz</trd>,
     <trd>Streben</trd>,
     <trd>Ambition</trd>
  </trdgrp>
  <trd lng="en">ambition</trd>
  <trd lng="es">ambici&oacute;n</trd>
  <trd lng="fr">ambition</trd>
  <trdgrp lng="nl">
    <trd>ambitie</trd>,
    <trd>eerzucht</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_m;&c_b;&c_i;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_ch;&c_je;&c_s;&c_t;&c_o;&c_l;&c_ju;&c_b;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="ambici.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Havanta <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>ehrgeizig</trd>,
     <trd>strebsam</trd>,
     <trd>ambitioniert</trd>
  </trdgrp>
  <trd lng="en">ambitious</trd>
  <trd lng="es">ambicioso</trd>
  <trd lng="fr">ambitieux</trd>
  <trd lng="nl">eerzuchtig</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_m;&c_b;&c_i;&c_c;&c_i;&c_o;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_ch;&c_je;&c_s;&c_t;&c_o;&c_l;&c_ju;&c_b;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="ambici.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>a pri:
      <ekz>
        mi ne <tld/>as tian gloron, al tia gloro.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Ehrgeiz zeigen</trd>,
     <trd>ehrgeizig sein</trd>,
     <trd>streben</trd>
  </trdgrp>
  <trd lng="en">have the ambition to</trd>
  <trdgrp lng="fr">
    <trd>avoir l'<ind>ambition</ind> (de)</trd>,
    <trd>ambitionner (de)</trd>,
    <trd>aspirer (&agrave;)</trd>
  </trdgrp>
  <trd lng="nl">ambi&euml;ren</trd>
</drv>
</art>
