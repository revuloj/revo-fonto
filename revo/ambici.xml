<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ambici.xml,v 1.22 2023/08/30 18:23:33 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>ambici</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ambici.0o">
  <kap><tld/>o</kap>
  <snc mrk="ambici.0o.PSI">
    <dif>
      <ref cel="am.glor0o">Gloramo</ref>, honoramo:
      <ekz>
        lia <tld/>o estis fari&gcirc;i grandfama, kaj la la&ubreve;doj estis por li necesa&jcirc;o
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia granda <tld/>o estas fari&gcirc;i unu tagon fama ar&hcirc;itektura artisto
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o ne permesis al li cedi al la volo de l&#x2019; homamasoj, kaj samtempe pro
        malkura&gcirc;eco li ne riskis kontra&ubreve;stari
        <fnt>
          <bib>IKr</bib>
          <lok>Quo Vadis?</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi mem estis malri&ccirc;a kaj <tld/>a, kiel vi, mi trairis eble samajn provojn, kiel
        vi
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de la junulo estis nur la senpera sekvo de ridegindaj revadoj de l&#x2019;
        infano
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="ambici.0o.KOMUNE">
    <dif>
      Afero aspirata, dezirata, celata pro <tld/>a sento:
      <ekz>
        partia <tld/>o
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Pri la homaranismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estas la deziro, per kiu &ccirc;iuj emocioj estas instigataj kaj plifortigataj
        kaj tial &ccirc;i tiu emocio povas esti regata tiel malfacile
        <fnt>
          <bib>IdO</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        politikaj <tld/>oj de subridantaj mistifikistoj
        <fnt>
          <aut>K. Kalocsay</aut>,
          <vrk>Mia penso</vrk>,
          <lok>Norda Prismo, 1957:4, p. 182a-185a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aspir.0o">aspiro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_b;&c_ib;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>ambice</trd>,
    <trd>aspirace</trd>,
    <trd>cti&zcaron;&aacute;dost</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Ehrgeiz</trd>,
     <trd>Streben</trd>,
     <trd>Ambition</trd>
  </trdgrp>
  <trd lng="en">ambition</trd>
  <trd lng="es">ambici&oacute;n</trd>
  <trd lng="fr">ambition</trd>
  <trd lng="he">&shin;&#x5C1;&#x5B0;&alef;&#x5B7;&pe;&tav;&#x5B8;&nun;&vav;&#x5BC;&tav;</trd>
  <trdgrp lng="ja">
    <trd>&#x91CE;&#x5FC3;  [<ind>&#x3084;&#x3057;&#x3093;</ind>]</trd>,
    <trd>&#x5927;&#x671B;  [<ind>&#x305F;&#x3044;&#x3082;&#x3046;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ambitie</trd>,
    <trd>eerzucht</trd>
  </trdgrp>
  <trd lng="pl">ambicja</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_m;&c_b;&c_i;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_ch;&c_je;&c_s;&c_t;&c_o;&c_l;&c_ju;&c_b;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>amb&iacute;cia</trd>,
    <trd>a&scaron;pir&aacute;cia</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&c_m;&c_b;&c_ib;&c_c;&c_ib;&c_ja;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_o;&c_l;&c_ju;&c_b;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_ch;&c_je;&c_s;&c_t;&c_o;&c_l;&c_ju;&c_b;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_v;&c_l;&c_a;&c_s;&c_t;&c_o;&c_l;&c_ju;&c_b;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_g;&c_o;&c_n;&c_o;&c_r;</trd>,
    <trd>&c_ch;&c_v;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_p;&c_i;&c_h;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="ambici.0a">
  <kap><tld/>a</kap>
  <snc mrk="ambici.0a.PSI">
    <dif>
      Havanta <tld/>on:
      <ekz>
        lia ri&ccirc;a&jcirc;o kaj povo <klr>[ja ne]</klr> transiru en la manojn de <tld/>aj
        friponoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>!
      </ekz>
      <ekz>
        kiel do <tld/>a, enviema, postulema kaj senbrida en siaj postuloj &scirc;i estis
        <fnt>
          <bib>Marta</bib>
        </fnt>!
      </ekz>
      <ekz>
        <tld/>aj homoj ne amas katojn, sed des pli la humilajn hundojn
        <fnt>
          <bib>IdO</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_b;&c_ib;&c_c;&c_y;&c_j;&c_n;&c_y;</trd>
  <trd lng="he">&shin;&#x5C1;&#x5B0;&alef;&#x5B7;&pe;&tav;&#x5B8;&nun;&yod;</trd>
  <trd lng="ja">&#x91CE;&#x5FC3;&#x7684;&#x306A;  [<ind>&#x3084;&#x3057;&#x3093;&#x3066;&#x304D;&#x306A;</ind>]</trd>
  <trd lng="ms">cita-cita</trd>
  <trdgrp lng="cs">
    <trd>ambici&oacute;zn&iacute;</trd>,
    <trd>n&aacute;ro&ccaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>ehrgeizig</trd>,
     <trd>strebsam</trd>,
     <trd>ambitioniert</trd>
  </trdgrp>
  <trd lng="en">ambitious</trd>
  <trd lng="es">ambicioso</trd>
  <trd lng="fr">ambitieux</trd>
  <trd lng="nl">eerzuchtig</trd>
  <trd lng="pl">ambitny</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_m;&c_b;&c_i;&c_c;&c_i;&c_o;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_ch;&c_je;&c_s;&c_t;&c_o;&c_l;&c_ju;&c_b;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ambici&oacute;zny</trd>,
    <trd>ambici&oacute;zny</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ch;&c_je;&c_s;&c_t;&c_o;&c_l;&c_ju;&c_b;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_l;&c_a;&c_s;&c_t;&c_o;&c_l;&c_ju;&c_b;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_a;&c_m;&c_b;&c_ib;&c_t;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_a;&c_m;&c_b;&c_ib;&c_c;&c_ib;&c_j;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_i;&c_h;&c_a;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="ambici.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="ambici.0i.PSI">
    <dif>
      Esti <tld/>a pri:
      <ekz>
        ni <tld/>as, &ccirc;u &ccirc;eestante a&ubreve; forestante, esti akceptindaj &ccirc;e <klr>[Dio]</klr>
        <fnt>
          <bib>NT</bib>
          <lok>II. Korintanoj 5:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        vera sa&gcirc;ulo estas tiu, kiu vivas modeste, ne <tld/>as, ne perfortas
        <fnt>
          <bib>Kandid</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne <tld/>as tian gloron, al tia gloro.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aspir.0i">aspiri</ref>,
      <ref cel="klopod.0i">klopodi</ref>,
      <ref cel="pen.0i">peni</ref>,
      <ref cel="preten.0i.postuli">pretendi<sncref/></ref>,
      <ref cel="streb.0i.KOMUNE">strebi<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_c;&c_mol; <ind>&c_a;&c_m;&c_b;&c_ib;&c_c;&c_y;&c_ju;</ind></trd>
  <trdgrp lng="cs">
    <trd>cti&zcaron;&aacute;dostiv&ecaron; usilovat</trd>,
    <trd>dychtit</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Ehrgeiz zeigen</trd>,
     <trd>ehrgeizig sein</trd>,
     <trd>streben</trd>
  </trdgrp>
  <trd lng="en">have the ambition to</trd>
  <trdgrp lng="fr">
    <trd>avoir l'<ind>ambition</ind> (de)</trd>,
    <trd>ambitionner (de)</trd>,
    <trd>aspirer (&agrave;)</trd>
  </trdgrp>
  <trd lng="he">&lamed;&shin;&alef;&vav;&fpe;</trd>
  <trd lng="ja">&#x5927;&#x671B;&#x3092;&#x3082;&#x3064;  [<ind>&#x305F;&#x3044;&#x3082;&#x3046;&#x3092;&#x3082;&#x3064;</ind>]</trd>
  <trd lng="nl">ambi&euml;ren</trd>
  <trdgrp lng="pl">
  <trd>by&cacute; <ind>ambitnym</ind></trd>,
  <trd>mie&cacute; <ind>ambicj&eogonek;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ma&tcaron; <ind>amb&iacute;ciu</ind></trd>,
    <trd>ma&tcaron; amb&iacute;ciu</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_o;&c_m;&c_a;&c_g;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_d;&c_o;&c_b;&c_i;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_g;&c_n;&c_u;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
</art>
