<drv mrk="stud.0o">
  <kap><tld/>o</kap>
  <snc mrk="stud.0o.KOMUNE">
    <dif>
      Ago <tld/>i, rezulto de tia ago
      <ekz>
        <tld/>oj kaj notoj pri la Internacia Lingvo
        <fnt><vrk><url ref="http://lingvakritiko.com/">Lingva Kritiko
        </url></vrk></fnt>;
      </ekz>
      <ekz>
        la <nom>Grimm</nom>'ajn fabelojn anka&ubreve; la maturaj homoj &scirc;atas <klr>[&#8230;]</klr> kiel
        materialon por folkloraj <tld/>oj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Anta&ubreve;parolo de la tradukinto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne bezonas pluajn teknikajn kaj teoriajn <tld/>ojn en la muziko
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ta&ubreve;gas nek por <tld/>o, nek por ludo
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
   </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_s;&c_mol;&c_l;&c_je;&c_d;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je; <klr>(&c_p;&c_r;&c_a;&c_c;&c_e;&c_s;)</klr></trd>,
    <trd>&c_v;&c_y;&c_v;&c_u;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>studie</trd>,
    <trd>v&ecaron;deck&aacute; n. v&yacute;tvarn&aacute; pr&aacute;ce</trd>,
    <trd>&scaron;tudie <klr>(hovor.)</klr></trd>
  </trdgrp>
  <trd lng="de">Studium</trd>
  <trd lng="fr">&eacute;tude</trd>
