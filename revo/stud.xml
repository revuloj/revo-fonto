<drv mrk="stud.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="stud.0i.objekton">
    <dif>
      Science esplori:
      <ekz>
        <tld/>i la proprecojn de iu substanco, iun vegeta&jcirc;on.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_a;&c_s;&c_l;&c_je;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>, 
      <trd>&c_v;&c_y;&c_v;&c_u;&c_ch;&c_a;&c_c;&c_mol;</trd> 
    </trdgrp>
    <trdgrp lng="hu">
      <trd>tanulm&aacute;nyoz</trd>,
      <trd>kutat</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>studiowa&cacute;</trd>,
      <trd>bada&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_i;&c_z;&c_u;&c_ch;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_i;&c_s;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="stud.0i.sciencon">
    <dif>
      Diligente labori por akiri la posedon de ia scienco:
      <ekz>
        <tld/>i fizikon, filozofion, ian specialan bran&ccirc;on.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_sh;&c_t;&c_u;&c_d;&c_z;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>, 
      <trd>&c_sh;&c_t;&c_u;&c_d;&c_y;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="hu">tanul <klr tip="ind">(fels&odblac;fokon)</klr></trd>
    <trdgrp lng="pl">
      <trd>studiowa&cacute;</trd>,
      <trd>zg&lstroke;&eogonek;bia&cacute; wiedz&eogonek;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_i;&c_z;&c_u;&c_ch;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_sh;&c_t;&c_u;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="af">studie</trd>
  <trd lng="am">&#x1325;&#x1293;&#x1275;</trd>
  <trd lng="ar">&#x62F;&#x631;&#x627;&#x633;&#x629;</trd>
  <trd lng="az">t&#x259;hsil</trd>
  <trd lng="bn">&#x997;&#x9AC;&#x9C7;&#x9B7;&#x9A3;&#x9BE;</trd>
  <trd lng="bo">&#xF66;&#xFA6;&#xFB1;&#xF7C;&#xF44; &#xF66;&#xFA6;&#xFB1;&#xF44; &#xF66;&#xFA6;&#xFB1;&#xF44;&#xF66; &#xF66;&#xFA6;&#xFB1;&#xF7C;&#xF44;&#xF66;&#xF0B;</trd>
  <trd lng="bs">studija</trd>
  <trd lng="co">studiu</trd>
  <trdgrp lng="cs">
    <trd>studovat</trd>,
    <trd>zkoumat</trd>,
    <trd>&scaron;tudovat <klr>(hovor.)</klr></trd>,
    <trd>&scaron;tud&yacute;rovat (hovor.)</trd>
  </trdgrp>
  <trd lng="da">unders&#xF8;gelse</trd>
  <trd lng="de">studieren</trd>
  <trd lng="en">study</trd>
  <trd lng="es">estudiar</trd>
  <trd lng="et">uuring</trd>
  <trd lng="eu">azterketa</trd>
  <trd lng="fr">&eacute;tudier</trd>
  <trd lng="fy">studearje</trd>
  <trd lng="gl">estudo</trd>
  <trd lng="gu">&#xA85;&#xAAD;&#xACD;&#xAAF;&#xABE;&#xAB8;</trd>
  <trd lng="hi">&#x905;&#x927;&#x94D;&#x92F;&#x92F;&#x928;</trd>
  <trd lng="hr">studija</trd>
  <trd lng="hy">&#x57D;&#x578;&#x57E;&#x578;&#x580;&#x565;&#x56C;</trd>
  <trd lng="id">bel<ind>ajar</ind></trd>
  <trd lng="is">ranns&#xF3;kn</trd>
  <trd lng="ja">&#x7814;&#x7A76;</trd>
