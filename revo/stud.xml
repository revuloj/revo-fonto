</drv>

<drv mrk="stud.0inda">
  <kap><tld/>inda</kap>
  <snc mrk="stud.0inda.KOMUNE">
    <dif>
      Kies <tld/>ado eble donos interesan rezulton:
      <ekz>
        jen ankora&ubreve; tre vasta, <tld/>inda kaj pliprofundiginda tereno
        <fnt>
          <aut>O. M. A&iuml;vanhov, trad. pluraj</aut>
          <vrk>La homo, konkere al sia destino</vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">&agrave; <ind>approfondir</ind></trd>
  <trd lng="pl">wart  <ind>studiowania</ind></trd>
</drv>

<drv mrk="stud.tra0i">
  <kap>
    tra<tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="stud.tra0i.PED">
    <dif>
      <tld lit="S"/>i tute, trarigardante &ccirc;ion:
      <ekz>
        tiu, kiu bone sin preparis, kiu serioze tra<tld/>is iun temon, estas &ccirc;iam pli
        forta ol tiu, kiu ne agis tiel
        <fnt>
          <aut>I. Lapenna</aut>
          <vrk>Retoriko</vrk>
          <lok>1950</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="pl">przestudiowa&cacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_u;&c_d;&c_ib;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_v;&c_ch;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_d;&c_o;&c_s;&c_l;&c_ib;&c_d;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
