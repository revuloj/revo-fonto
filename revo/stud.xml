  <trd lng="ka">&#x10E1;&#x10D0;&#x10E1;&#x10EC;&#x10D0;&#x10D5;&#x10DA;&#x10DD;</trd>
  <trd lng="km">&#x1780;&#x17B6;&#x179A;&#x179F;&#x17B7;&#x1780;&#x17D2;&#x179F;&#x17B6;</trd>
  <trd lng="ko">&#xC5F0;&#xAD6C;</trd>
  <trd lng="lo">&#xE81;&#xEB2;&#xE99;&#xEAA;&#xEB6;&#xE81;&#xEAA;&#xEB2;</trd>
