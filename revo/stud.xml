<drv mrk="stud.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="stud.0ajxo.KOMUNE">
    <dif>
      Verko resumanta la rezultojn de <tld/>ado:
      <ekz>
        tiu &ccirc;i scienculo balda&ubreve; publikigos esperante rimarkindan
        <tld/>a&jcirc;on
        <fnt>
          <aut>T. Cart</aut>
          <vrk><url ref="http://arkivo.esperanto-france.org/divers/bildaro/zoolog.htm">
          Instruado de Esperanto per bildaro</url></vrk>
          <lok>1905</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">&eacute;tude<klr> (ouvrage)</klr></trd>
  <trd lng="ja">&#30740;&#x7A76;  <pr>&#12369;&#12435;&#x304D;&#12421;&#12358;</pr></trd>
  <trd lng="pl">studium</trd>
  <trdgrp lng="uk">
    <trd>&c_n;&c_a;&c_u;&c_k;&c_o;&c_v;&c_a; &c_r;&c_o;&c_z;&c_r;&c_o;&c_b;&c_k;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_u;&c_k;&c_o;&c_v;&c_a; &c_p;&c_r;&c_a;&c_c;&c_ja;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_v;&c_ib;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_d;&c_o;&c_s;&c_l;&c_ib;&c_d;&c_zh;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_u;&c_d;&c_ib;&c_ji;</trd>
