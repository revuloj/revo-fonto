<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>nokturn</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="nokturn.0o">
  <kap><tld/>o</kap>
  <snc mrk="nokturn.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="kompon.0ajxo">Kompona&jcirc;o</ref> inspirita de
      nokta etoso, ordinare delikata, malgaja, en romantikisma epoko
      precipe piana:
      <ekz>
        Marta <klr>...</klr> komencis ludi la <tld lit="N"/>on de
        Zientarski
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_k;&c_t;&c_u;&c_r;&c_n;</trd>
  <trd lng="es">nocturno <klr tip="amb"> (sust., m&uacute;s.)</klr></trd>
  <trd lng="fr">nocturne<klr tip="amb"> (subst., musique)</klr></trd>
</drv>
</art>

