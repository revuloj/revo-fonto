<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nokturn.xml,v 1.14 2024/01/29 15:59:31 revo Exp $">
<kap>
  <rad>nokturn</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="nokturn.0o">
  <kap><tld/>o</kap>
  <snc mrk="nokturn.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="kompon.0ajxo">Kompona&jcirc;o</ref> inspirita de
      nokta etoso, ordinare delikata, malgaja, en romantikisma epoko
      precipe piana:
      <ekz>
        Marta <klr>...</klr> komencis ludi la <tld lit="N"/>on de
        Zientarski
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_k;&c_t;&c_u;&c_r;&c_n;</trd>
  <trd lng="cs">nokturno</trd>
  <trdgrp lng="de">
    <trd>Nocturne</trd>,
    <trd>Nokturne</trd>,
    <trd>Nachtmusik</trd>
  </trdgrp>
  <trd lng="en">nocturne</trd>
  <trd lng="es">nocturno <klr tip="amb"> (sust., m&uacute;s.)</klr></trd>
  <trd lng="fr">nocturne<klr tip="amb"> (subst., musique)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x591C;&#x60F3;&#x66F2;  <pr>&#x3084;&#x305D;&#x3046;&#x304D;&#x3087;&#x304F;</pr></trd>,
    <trd>&#x30CE;&#x30AF;&#x30BF;&#x30FC;&#x30F3;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>nocturne</trd>,
    <trd>nachtmuziek</trd>
  </trdgrp>
  <trd lng="pl">nokturn</trd>
  <trd lng="ru">&c_n;&c_o;&c_k;&c_t;&c_ju;&c_r;&c_n;</trd>
  <trd lng="sk">nokturno</trd>
  <trd lng="uk">&#x43D;&#x43E;&#x43A;&#x442;&#x44E;&#x440;&#x43D;</trd>
  <trd lng="zh">&#x591C;&#x66F2; <pr>y&#xE8;q&#x1D4;</pr></trd>
</drv>

</art>

