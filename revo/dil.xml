<vortaro>
  <art mrk="$Id: dil.xml,v 1.4 2025/06/02 20:00:22 afido Exp $">
    <kap>
      <rad>Dil</rad>/o
      <fnt><bib>PIV2</bib></fnt>
    </kap>

<drv mrk="dil.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&Ccirc;efurbo</ref>
      de <ref tip="malprt" cel="timor.OrientaT0o">Orienta Timoro</ref>:
      <ekz>
        <tld/>o situas &ccirc;e la norda marbordo de insulo Timoro, la plej orienta en la
        malaja arkipelago
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=34844">Dilo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        el la kombina&jcirc;o de okupado kaj konflikto, kaj la sekvoj <klr>[&#8230;]</klr> ni nun
        vidas ellasitaj sur la stratoj de <tld/>o
        <fnt>
          <aut>Angela Robson </aut>
          <vrk><url ref="https://eo.mondediplo.com/2008/02/article1363.html">En Orienta Timoro, la
          estinteco minacas la estantecon</url></vrk>
          <lok> Le Monde diplomatique en Esperanto, Feb 2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Dili</trd>
    <trd lng="en">Dili</trd>
    <trd lng="fr">Dili</trd>
    <trd lng="ru">&c_D;&c_i;&c_l;&c_i;</trd>
  </snc>
  <trd lng="be">&c_D;&c_y;&c_l;&c_ib; <klr>(&c_s;&c_t;&c_a;&c_l;&c_ib;&c_c;&c_a;
  &c_W;&c_s;&c_h;&c_o;&c_d;&c_n;&c_ja;&c_g;&c_a; &c_T;&c_y;&c_m;&c_o;&c_r;&c_u;)</klr></trd>
  <trd lng="ca">Dili</trd>
  <trd lng="es">Dili</trd>
  <trd lng="ja">&#12487;&#12451;&#12522;</trd>
</drv>

  </art>

