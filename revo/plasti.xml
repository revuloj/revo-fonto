<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>plastik</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="plasti.0o">
  <kap><tld/>o</kap>
  <snc mrk="plasti.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Arto reliefe reprodukti la formojn de objektoj.
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>pl&agrave;stica</trd>,
    <trd>modelatge</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Plastik</trd>,
    <trd>Bildhauerkunst</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>plastic art</trd>,
    <trd>modeling art</trd>,
    <trd>molding art</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>Pl&aacute;stica</trd>,
    <trd>Artes Pl&aacute;sticas</trd>
  </trdgrp>
  <trd lng="fr">arts plastiques</trd>
  <trd lng="hu">plasztika</trd>
  <trdgrp lng="nl">
    <trd>plastiek</trd>,
    <trd>boetseerkunst</trd>,
    <trd>beeldhouwkunst</trd>
  </trdgrp>
  <trd lng="pl">plastyka</trd>
  <trd lng="pt">arte de <ind>plasmar</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_a;&c_s;&c_t;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_a;&c_ja;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_k;&c_u;&c_l;&c_mol;&c_p;&c_t;&c_u;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">plastika</trd>
</drv>

<drv mrk="plasti.0a">
  <kap><tld/>a</kap>
  <snc mrk="plasti.0a.BELA">
    <dif>
      Rilata al <tld/>o, ta&ubreve;ga por <tld/>o:
      <ekz>
        <tld/>a (knedebla) pasto.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>formbar</trd>,
      <trd>knetbar</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>plastic<klr tip="amb"> (artistic)</klr></trd>,
      <trd>modeling</trd>,
      <trd>molding</trd>
    </trdgrp>
    <trd lng="fr">plastique<klr> (adj., arts)</klr></trd>
    <trd lng="pl">plastyczny</trd>
    <trd
    lng="ru">&c_p;&c_l;&c_a;&c_s;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
  <snc mrk="plasti.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Reliefa kiel la reala&jcirc;o; havanta reliefajn formojn precize
      similantajn la naturon; same impresanta kiel la reala&jcirc;o:
      <ekz>
        <tld/>e imagi ion al si<fnt>B</fnt>;
      </ekz>
      <ekz>
        &ccirc;io staras klare kaj <ind><tld/>e</ind> anta&ubreve; mia
        animo<fnt>Z</fnt>;
        <trdgrp lng="ca">
          <trd>tangiblement</trd>,
          <trd>gaireb&eacute; real</trd>
        </trdgrp>
      </ekz>
      <ekz>
        la silueto de la parlamento <tld/>e reliefi&gcirc;as
        kontra&ubreve; la vespera
        &ccirc;ielo;
      </ekz>
      <ekz>
        <tld/>e esprimi ion;
      </ekz>
      <ekz>
        <tld/>e kaj emocie priskribi la mizeran situacion de
        iu<fnt>B</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="drast">draste</ref>,
      <ref cel="viv">vive</ref>
    </refgrp>
    <trd lng="de">plastisch</trd>
    <trd lng="es">pl&aacute;stico, -a<klr> (adj.)</klr></trd>
    <trd lng="pl">plastyczny</trd>
  </snc>
  <trdgrp lng="ca">
    <trd>pl&agrave;stic</trd>,
    <trd>modelable</trd>,
    <trd>deformable</trd>
  </trdgrp>
  <trd lng="hu">plasztikus</trd>
  <trd lng="nl">plastisch</trd>
</drv>
</art>
