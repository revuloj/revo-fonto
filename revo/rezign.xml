<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc> <rad>rezign</rad>/i
</kap>
<drv mrk="rezign.0i">
  <kap><tld/>i</kap>
  <snc>
    <gra><vspec>x</vspec></gra>
    <dif>
      Definitive forlasi ian postulon, pretendon, esperon a&ubreve; deziron;
      malpretendi:
      <ekz>
        <tld/>i rajton, la tronon, entreprenon, sian programom;
      </ekz>
      <ekz>
        <tld/>i pri io;
      </ekz>
      <ekz>
        mi tute <tld/>as esti &gcirc;uste komprenata de ili.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="abdik.0i">abdiki</ref>,
    <ref cel="eks.0igxi">eksi&gcirc;i</ref>,
    <ref cel="jxet.for0i">for&jcirc;eti</ref>,
    <ref cel="las.for0i">forlasi</ref>,
    <ref cel="met.for0i">formeti</ref>.
  </refgrp>
  <trdgrp lng="de">
      <trd>aufgeben</trd>,
      <trd>resignieren</trd>
  </trdgrp>
  <trd lng="en">resign</trd>
  <trdgrp lng="fr">
    <trd>abandonner<klr> (renoncer)</klr></trd>,
    <trd>renoncer</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>afstand doen</trd>,
    <trd>opgeven</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol;&c_s;&c_ja; <klr>(&c_o;&c_t; &c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>,
    <trd>&c_o;&c_t;&c_r;&c_je;&c_ch;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="rezign.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <trdgrp lng="de">
      <trd>Aufgabe</trd>,
      <trd>Resignation</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>abandon<klr> (renonciation)</klr></trd>,
    <trd>renonciation</trd>
  </trdgrp>
  <trd lng="nl">afstand<klr tip="ind"> (opgeven)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_r;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_o;&c_t;&c_k;&c_a;&c_z; <klr>(&c_o;&c_t; &c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="rezign.0emo">
  <kap><tld/>emo</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="rezign1.0o">Rezignacio</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
      <trd>Schicksalsergebenheit</trd>,
      <trd>Resignation</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>r&eacute;signation</trd>
  </trdgrp>
  <trd lng="nl">berusting</trd>
  <trd lng="ru">&c_s;&c_m;&c_i;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>
</art>
