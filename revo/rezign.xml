<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc> <rad>rezign</rad>/i
</kap>
<drv mrk="rezign.0i">
  <kap><tld/>i</kap>
  <snc>
    <gra><vspec>x</vspec></gra>
    <dif>
      Definitive forlasi ian postulon, pretendon, esperon a&ubreve; deziron;
      malpretendi:
      <ekz>
        <tld/>i rajton, la tronon, entreprenon, sian programom;
      </ekz>
      <ekz>
        <tld/>i pri io;
      </ekz>
      <ekz>
        mi tute <tld/>as esti &gcirc;uste komprenata de ili.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="jxet.for0i">for&jcirc;eti</ref>,
    <ref cel="las.for0i">forlasi</ref>,
    <ref cel="met.for0i">formeti</ref>.
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_m;&c_o;&c_v;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_a;&c_d;&c_r;&c_a;&c_ch;&c_y;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_y;&c_r;&c_a;&c_ch;&c_y;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_r;&c_a;&c_ch;&c_y;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>resignar</trd>,
    <trd>abandonar</trd>,
    <trd>renunciar a</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>aufgeben</trd>,
    <trd>resignieren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>resign</trd>,
    <trd>give up</trd>,
    <trd>surrender</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>abandonner<klr> (renoncer)</klr></trd>,
    <trd>renoncer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>lemond</trd>,
    <trd>felad</trd>,
    <trd>el&aacute;ll <klr> (vmit&odblac;l)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>afstand doen</trd>,
    <trd>opgeven</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rezygnowa&cacute;</trd>,
    <trd>zreka&cacute; si&eogonek;</trd>,
    <trd>wyrzeka&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>resignar-se</trd>,
    <trd>abrir m&atilde;o de</trd>,
    <trd>renunciar</trd>,
    <trd>demitir-se de</trd>,        
    <trd>abandonar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol;&c_s;&c_ja; <klr>(&c_o;&c_t; &c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>,
    <trd>&c_o;&c_t;&c_r;&c_je;&c_ch;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="tr">vazge&ccedil;mek</trd>
  <trdgrp lng="sk">
    <trd>dostúpiť</trd>,
    <trd>podať demisiu</trd>,
    <trd>rezignovať</trd>,
    <trd>vzdať sa</trd>,
    <trd>zriecť sa</trd>
  </trdgrp>
</drv>

<drv mrk="rezign.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_r;&c_a;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_a;&c_d;&c_m;&c_o;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>ren&uacute;ncia</trd>,
    <trd>aband&oacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Aufgabe</trd>,
    <trd>Resignation</trd>
  </trdgrp>
  <trd lng="en">resignation</trd>
  <trdgrp lng="fr">
    <trd>abandon<klr> (renonciation)</klr></trd>,
    <trd>renonciation</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>lemond&aacute;s</trd>,
    <trd>felad&aacute;s</trd>,
    <trd>belenyugv&aacute;s</trd>,
    <trd>rezign&aacute;ci&oacute;</trd>
  </trdgrp>
  <trd lng="nl">afstand<klr tip="ind"> (opgeven)</klr></trd>
  <trdgrp lng="pl">
    <trd>rezygnacja</trd>,
    <trd>zrzeczenie si&eogonek;</trd>,
    <trd>ust&aogonek;pienie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_r;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_o;&c_t;&c_k;&c_a;&c_z; <klr>(&c_o;&c_t; &c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odstúpenie z miesta</trd>,
    <trd>rezignácia</trd>,
    <trd>vzdanie sa</trd>,
    <trd>zrieknutie sa</trd>
  </trdgrp>
</drv>

<drv mrk="rezign.0emo">
  <kap><tld/>emo</kap>
  <snc>
    <ref tip="dif" cel="rezign1.0o">rezignacio</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_k;&c_o;&c_r;&c_a;</trd>
  <trd lng="ca">resignaci&oacute;</trd>
  <trdgrp lng="de">
    <trd>Schicksalsergebenheit</trd>,
    <trd>Resignation</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>resignation</trd>,
    <trd>defeatism</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>r&eacute;signation</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>belet&ouml;r&odblac;d&eacute;s</trd>,
    <trd>rezign&aacute;lts&aacute;g</trd>
  </trdgrp>
  <trd lng="nl">berusting</trd>
  <trdgrp lng="pl">
    <trd>rezygnacja</trd>,
    <trd>defetyzm</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_m;&c_i;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">ústupčivosť</trd>
</drv>
</art>
