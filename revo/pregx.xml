<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pre&gcirc;</rad>/i
</kap>
<drv mrk="pregx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Adori Dion, petante favoron a&ubreve; dankante pro favoro:
      <ekz>
        <tld/>u al la Sankta Virgulino<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>u kore kaj laboru fervore<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i por<fnt>Z</fnt>,
      </ekz>
      <ekz>
        pri<fnt>Z</fnt>,
      </ekz>
      <ekz>
        pro iu<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>is sian vesperan <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        a&ubreve;skultu la <tld/>on, per kiu via servanto <tld/>as anta&ubreve; Vi
        hodia&ubreve;<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">beten</trd>
  <trd lng="en">pray</trd>
  <trd lng="fr">prier</trd>
  <trd lng="nl">bidden</trd>
  <trd lng="pl">modli&cacute; si&eogonek;</trd>
  <trd lng="ru">&c_m;&c_o;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="sv">bedja</trd>
</drv>

<drv mrk="pregx.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Konstruo en kiu oni <tld/>as:
      <ekz>
        malri&ccirc;a kiel muso <tld/>eja<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Kirche</trd>
  <trd lng="en">church</trd>
  <trd lng="es">iglesia</trd>
  <trd lng="fr">&eacute;glise</trd>
  <trd lng="nl">kerk</trd>
  <trd lng="pl">ko&sacute;ci&oacute;&lstroke;</trd>
  <trd lng="ru">&c_c;&c_je;&c_r;&c_k;&c_o;&c_v;&c_mol;</trd>
  <trd lng="sv">kyrka</trd>
</drv>
</art>
