<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>pre&gcirc;</rad>/i
    </kap>

    <drv mrk="pregx.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <uzo tip="fak">REL</uzo>
      <snc>
	<dif>
	  Sin turni al dio, diino a&ubreve; alia objekto de kulto,
	  petante ties protekton, favoron a&ubreve; dankante pro
	  favoro:
	  <ekz><tld/>u al la Sankta Virgulino<fnt><bib>F</bib>, <lok>&FE; 37</lok></fnt>;
	  </ekz><ekz>
	    <tld/>u kore kaj laboru fervore
	    <fnt><bib>PrV</bib></fnt>;
	  </ekz><ekz>
	    <tld/>i por<fnt>Z</fnt>,
	  </ekz><ekz>
	    pri<fnt>Z</fnt>,
	  </ekz><ekz>
	    pro iu<fnt>Z</fnt>;
	  </ekz><ekz>
	    &scirc;i <tld/>is sian vesperan <tld/>on<fnt>Z</fnt>;
	  </ekz><ekz>
	    a&ubreve;skultu la <tld/>on, per kiu via servanto <tld/>as anta&ubreve; Vi
	    hodia&ubreve;<fnt>Z</fnt>.
	  </ekz>
	</dif>
      </snc>
      <trd lng="be">&c_m;&c_a;&c_l;&c_ib;&c_c;&c_c;&c_a;</trd>
      <trd lng="de">beten</trd>
      <trd lng="en">pray</trd>
      <trd lng="fr">prier</trd>
      <trd lng="hu">im&aacute;dkozik</trd>
      <trd lng="nl">bidden</trd>
      <trd lng="pl">modli&cacute; si&eogonek;</trd>
      <trd lng="ru">&c_m;&c_o;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
      <trd lng="sv">bedja</trd>
    </drv>

    <drv mrk="pregx.0ejo">
      <kap><tld/>ejo</kap>
      <uzo tip="fak">REL</uzo>
      <snc>
	<dif>
	  Konstrua&jcirc;o a&ubreve; loko en kiu oni <tld/>as:
	  <ekz>
	    la graciaj turpintoj de la <tld/>ejo levi&gcirc;as super
	    la malalta urbo kaj speguli&gcirc;as en Isefjord
	    <fnt><bib>Fab4</bib>, <lok>Peco da perlovico</lok></fnt>;
	  </ekz><ekz>
	    mi venigos sur Mian sanktan monton kaj &gcirc;ojigos en Mia
	    <tld/>ejo; iliaj bruloferoj kaj bu&ccirc;oferoj estos favore
	    akceptataj sur Mia altaro; &ccirc;ar Mia domo
	    estos nomata <tld/>ejo por &ccirc;iuj popoloj
	    <fnt><bib>MT</bib>, <lok>&Jes; 56:7</lok></fnt>;
	  </ekz><ekz>
	    malri&ccirc;a kiel muso <tld/>eja
	    <fnt><bib>PrV</bib></fnt>.
	  </ekz>
	</dif>
      </snc>
      <ref tip="sin" cel="templ.0o.kultejo">templo<sncref/></ref>
      <ref tip="super" cel="sankt.0ejo">sanktejo</ref>
      <refgrp tip="sub">
	<ref cel="kapel.0o.neregula">kapelo<sncref/></ref>,
	<ref cel="kirk.0o">kirko</ref>,
	<ref cel="moske.0o">moskeo</ref>,
	<ref cel="sinago.0o.pregxejo">sinagogo<sncref/></ref>
	<ref cel="templ.0o.ejo">templo<sncref/></ref>
      </refgrp>
      <trdgrp lng="be">
        <trd>&c_b;&c_a;&c_zh;&c_n;&c_ib;&c_c;&c_a;</trd>
        <trd>&c_m;&c_a;&c_l;&c_je;&c_l;&c_mol;&c_n;&c_ja;</trd>,
        <trd>&c_m;&c_a;&c_l;&c_je;&c_l;&c_mol;&c_n;&c_y; &c_d;&c_o;&c_m;</trd>,
        <trd>&c_c;&c_a;&c_r;&c_k;&c_v;&c_a;</trd>,
        <trd>&c_k;&c_a;&c_s;&c_mol;&c_c;&c_jo;&c_l;</trd>,
        <trd>&c_k;&c_ib;&c_r;&c_h;&c_a;</trd>,
        <trd>&c_m;&c_ja;&c_ch;&c_e;&c_c;&c_mol;</trd>,
        <trd>&c_m;&c_ja;&c_ch;&c_e;&c_t;</trd>,
        <trd>&c_d;&c_zh;&c_a;&c_m;&c_ib;&c_ja;</trd>
      </trdgrp>
      <trd lng="de">Kirche</trd>
      <trdgrp lng="de">
	<trd>Gebetshaus</trd>,
	<trd>Kirche<klr> [kristana]</klr></trd>
      </trdgrp>
      <trd lng="en">church</trd>
      <trd lng="es">iglesia</trd>
      <trd lng="fr">&eacute;glise</trd>
      <trd lng="hu">templom</trd>
      <trd lng="nl">kerk</trd>
      <trd lng="pl">ko&sacute;ci&oacute;&lstroke;</trd>
      <trdgrp lng="ru">
	<trd>&c_m;&c_o;&c_l;&c_je;&c_l;&c_mol;&c_n;&c_ja;</trd>;
	<trd>&c_c;&c_je;&c_r;&c_k;&c_o;&c_v;&c_mol;<klr> (kristana)</klr></trd>
      </trdgrp>
      <trd lng="sv">kyrka</trd>
    </drv>

    <drv mrk="pregx.hejma0ejo">
      <kap>hejma <tld/>ejo</kap>
      <uzo tip="fak">REL</uzo>
      <snc>
	<dif>
	  <tld lit="P"/>ejo aran&gcirc;ita en domo a&ubreve; palaco de mo&scirc;tulo:
	  <ekz>
	    la eklezio diversmaniere limigis la diservojn kiujn oni
	    povas fari en la hejmaj <tld/>ejoj;
	  </ekz><ekz>
	    sur la tegmento de la Vintra palaco en Peterburgo
	    oni vidas la malgrandan kupolon de la hejma <tld/>ejo
	    de la imperiestra familio.
	  </ekz>
	</dif>
      </snc>
      <trd lng="en"><klr tip="amb">(private) </klr><ind>oratory</ind></trd>
      <trd lng="ru">&c_d;&c_o;&c_m;&c_o;&c_v;&c_a;&c_ja; &c_c;&c_je;&c_r;&c_k;&c_o;&c_v;&c_mol;</trd>
    </drv>

    <drv mrk="pregx.0olibro">
      <kap><tld/>olibro</kap>
      <uzo tip="fak">REL</uzo>
      <snc>
	<dif>
	  Libro entenanta <tld/>ojn:
	  <ekz>
	    nur tra la malgranda kornatavola fenestro de tegmenta
	    &ccirc;ambreto penetradis eksteren senbrila lumeto, tie la
	    maljuna sinjoro ordinare sidis sur sia lito kun sia
	    germana <tld/>olibro en la mano kaj kantis sian vesperan
	    <tld/>on
	    <fnt><bib>Fab3</bib>, <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok></fnt>.
	  </ekz>
	</dif>
      </snc>
      <ref tip="vid" cel="rit.0libro">ritlibro</ref>
      <trd lng="en">prayer-book</trd>
      <trd lng="ru">&c_m;&c_o;&c_l;&c_i;&c_t;&c_v;&c_je;&c_n;&c_n;&c_i;&c_k;</trd>
    </drv>

  </art>
