<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>administr</rad>/i
</kap>
<drv mrk="admini.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Prizorgi la funkciadon de publika a&ubreve; grava privata
      organizo:
      <ekz>
        <tld/>i societon, distrikton, ministrejon.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">administer</trd>
  <trd lng="es">administrar</trd>
  <trd lng="fr">administrer</trd>
  <trd lng="it">amministrare</trd>
  <trd lng="nl">beheren</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_u;&c_k;&c_o;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_p;&c_r;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
    <trd lng="sk">spravova&tcaron;</trd>
</drv>

<drv mrk="admini.0antaro">
  <kap><tld/>antaro</kap>
  <snc>
    <dif>
      Grupo de personoj, kiuj <tld/>as ion.
    </dif>
    <refgrp tip="sin">
      <ref cel="admini1.0o.ularo">administracio</ref>
    </refgrp>
    <trd lng="en">administrative unit</trd>
    <trd lng="es">administraci&oacute;n</trd>    
    <trd lng="fr">administration<klr> (groupe, institution)</klr></trd>
    <trdgrp lng="hu">
      <trd>igazgat&oacute;s&aacute;g</trd>,
      <trd>vezet&odblac;s&eacute;g</trd>,
      <trd>adminsztr&aacute;ci&oacute;</trd>
    </trdgrp>
    <trd lng="nl">raad van beheer</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_u;&c_k;&c_o;&c_v;&c_o;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>,
      <trd>&c_a;&c_d;&c_m;&c_i;&c_n;&c_i;&c_s;&c_t;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>spr&aacute;va</trd>,
      <trd>administrat&iacute;va <klr>(&uacute;radn&iacute;ci)</klr></trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="admini.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Loko en kiu sidas administrantaro.
    </dif>
    <refgrp tip="sin">
      <ref cel="admini1.0o.ejo">administracio</ref>
    </refgrp>
    <trd lng="en">administrative office</trd>
    <trd lng="es">administraci&oacute;n</trd>
    <trd lng="fr">administration<klr> (batiment)</klr></trd>
    <trd lng="hu">igazgat&oacute;s&aacute;gi <ind>&eacute;p&uuml;let</ind></trd>
    <trd lng="nl">administratie</trd>
    <trd lng="ru">&c_a;&c_d;&c_m;&c_i;&c_n;&c_i;&c_s;&c_t;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
    <trd lng="sk">administr&aacute;cia <klr>(&uacute;rad)</klr></trd>
  </snc>
</drv>
</art>
