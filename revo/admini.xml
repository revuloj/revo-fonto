    <trd>&#x43A;&c_je;&c_r;&c_ib;&c_v;&#x43D;&c_i;&#x43A;</trd>,
    <trd>&c_a;&c_d;&#x43C;&c_ib;&#x43D;&c_ib;&c_s;&c_t;&c_r;&c_a;&c_t;&#x43E;&c_r;</trd>
