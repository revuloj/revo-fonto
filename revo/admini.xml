<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>administr</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="admini.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="admini.0i.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Prizorgi la funkciadon de publika a&ubreve; grava privata
      organizo:
      <ekz>
        <tld/>i societon, distrikton, ministrejon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_m;&c_ib;&c_n;&c_ib;&c_s;&c_t;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>melestri&ntilde;</trd>,
    <trd>mera&ntilde;</trd>
  </trdgrp>
  <trd lng="de">verwalten</trd>
  <trd lng="en">administer</trd>
  <trd lng="es">administrar</trd>
  <trdgrp lng="fr">
    <trd>administrer<klr tip="ind"> (diriger)</klr></trd>,
    <trd>diriger</trd>,
    <trd>g&eacute;rer</trd>,
    <trd>manager</trd>,
    <trd>piloter</trd>
  </trdgrp>
  <trd lng="it">amministrare</trd>
  <trd lng="nl">beheren</trd>
  <trdgrp lng="pt">
    <trd>administrar</trd>,
    <trd>gerir</trd>
    <trd>reger</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_u;&c_k;&c_o;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_p;&c_r;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">spravova&tcaron;</trd>
  <trd lng="sv">administrera</trd>  
</drv>

<drv mrk="admini.0antaro">
  <kap><tld/>antaro</kap>
  <snc mrk="admini.0antaro.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Grupo de personoj, kiuj <tld/>as ion.
    </dif>
    <refgrp tip="sin">
      <ref cel="admini1.0o.ularo">administracio <sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_a;&c_d;&c_m;&c_ib;&c_n;&c_ib;&c_s;&c_t;&c_r;&c_a;&c_c;&c_y;&c_ja;</trd>,
      <trd>&c_k;&c_ib;&c_r;&c_a;&c_w;&c_n;&c_ib;&c_c;&c_t;&c_v;&c_a;</trd>
    </trdgrp>
      <trd lng="br">melestradurezh<klr tip="ind"> (an dud, an
      ensavadur)</klr>
    </trd>
    <trd lng="de">Verwaltung</trd>
    <trd lng="en">administrative unit</trd>
    <trd lng="es">administraci&oacute;n</trd>    
    <trd lng="fr">administration<klr tip="ind"> (groupe,
    institution)</klr></trd>
    <trdgrp lng="hu">
      <trd>igazgat&oacute;s&aacute;g</trd>,
      <trd>vezet&odblac;s&eacute;g</trd>,
      <trd>adminsztr&aacute;ci&oacute;</trd>
    </trdgrp>
    <trd lng="it">amministrazione<klr> (dirigenza)</klr></trd>
    <trd lng="nl">raad van beheer</trd>
    <trd lng="pt">Adminstra&ccedil;&atilde;o<klr>(grupo de pessoas que
    administram)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_u;&c_k;&c_o;&c_v;&c_o;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>,
      <trd>&c_a;&c_d;&c_m;&c_i;&c_n;&c_i;&c_s;&c_t;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>spr&aacute;va</trd>,
      <trd>administrat&iacute;va <klr>(&uacute;radn&iacute;ci)</klr></trd>
    </trdgrp>
    <trd lng="sv">administration<klr tip="amb"> (grupp,
    institution)</klr></trd>
  </snc>
</drv>

<drv mrk="admini.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="admini.0ejo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Loko en kiu oficas administrantaro:
      <ekz>
        la <tld/>ejo de la lernejo estis en la unua eta&gcirc;o
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>, 
        <lok>&ccirc;apitro 34a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="admini1.0o.ejo">administracio <sncref/></ref>
    </refgrp>
    <trd lng="be">&c_a;&c_d;&c_m;&c_ib;&c_n;&c_ib;&c_s;&c_t;&c_r;&c_a;&c_c;&c_y;&c_ja;</trd>
    <trd lng="br">melestradurezh<klr tip="ind"> (al lec'h)</klr></trd>
    <trd lng="de">Verwaltungsgeb&auml;ude</trd>
    <trd lng="en">administrative office</trd>
    <trd lng="es">administraci&oacute;n</trd>
    <trd lng="fr">administration<klr tip="ind"> (b&acirc;timent)</klr></trd>
    <trd lng="hu">igazgat&oacute;s&aacute;gi <ind>&eacute;p&uuml;let</ind></trd>
    <trd lng="it">amministrazione<klr> (sede)</klr></trd>
    <trd lng="nl">administratie</trd>
    <trd lng="pt">sede de administra&ccedil;&atilde;o</trd>
    <trd lng="ru">&c_a;&c_d;&c_m;&c_i;&c_n;&c_i;&c_s;&c_t;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
    <trd lng="sk">administr&aacute;cia <klr>(&uacute;rad)</klr></trd>
  </snc>
</drv>
</art>
