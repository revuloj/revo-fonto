<drv mrk="admini.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="admini.0i.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Prizorgi la funkciadon de publika a&ubreve; grava privata
      organizo:
      <ekz>
        <tld/>i financojn, bienon, societon, distrikton, ministrejon;
      </ekz>
      <ekz>
        Dario bonvolis starigi super sia regno cent dudek satrapojn, por <tld/>i lian
        tutan regnon
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 6:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> estroj de la triboj <klr>[&#8230;]</klr> <tld/>is la kalkuladon
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 7:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        via edzo estas posedanto de la magazeno en rilato jura, sed vi <tld/>as la
        aferojn kune kun li
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la unua ministro <klr>[&#8230;]</klr> donas al &ccirc;iuj konsilojn, kiamaniere oni devas
        <tld/>i la &scirc;taton
        <fnt>
          <bib>IKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        la nova maniero <tld/>i la bienon knaris kiel ne&scirc;mirita rado
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>as la kristanan komunumon
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Vilmos Bleier</nom> <klr>[&#8230;]</klr> kompetente <tld/>is la financojn de Literatura Mondo
        <fnt>
          <bib>EeP</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mastr.0umi">mastrumi</ref>,
      <ref cel="rigard.super0i.FIG">superrigardi<sncref/></ref>,
      <ref cel="ten.0i.mastrume">teni<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_m;&c_ib;&c_n;&c_ib;&c_s;&c_t;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&t_da;&t_naro;&t_tsheg;&t_da;&t_ma;&t_tsheg;</trd>
  <trdgrp lng="br">
    <trd>melestri&ntilde;</trd>,
    <trd>mera&ntilde;</trd>
  </trdgrp>
  <trd lng="cs">&scaron;af&aacute;&rcaron;ovat</trd>
