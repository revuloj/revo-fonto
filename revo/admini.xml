<drv mrk="admini.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="admini.0ejo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Loko en kiu oficas administrantaro;
      <ref tip="sin" cel="admini1.0o.ejo">administracio<sncref/></ref>:
      <ekz>
        la <tld/>ejo de la lernejo estis en la unua eta&gcirc;o
        <fnt><aut>I. G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>,
        <lok>[1898?]</lok></fnt>;
      </ekz>
      <ekz>
        por ricevi provekzempleron skribu al la <tld/>ejo <klr>[&#8230;]</klr> a&ubreve; al la
        redakcio
        <fnt>
          <bib>LOdE</bib>
          <lok> 2000, No 10</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni veturas, la&ubreve; oficistoj de la prezidanta <tld/>ejo, kvaza&ubreve; per kamiono kun
        &scirc;tata flago
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2007/009261.php">Spartana sorto</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ministra kabineto malpermesis renovigon, riparon kaj rekonstruadon de
        <tld/>ejoj
        <fnt>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2000/006806.php">&Ccirc;iea &scirc;parado</url></vrk>
          <bib>Monato</bib>
          <lok>2000/05, p. 7</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ofic.0ejo">oficejo</ref>,
      <ref cel="stab.0ejo">stabejo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_m;&c_ib;&c_n;&c_ib;&c_s;&c_t;&c_r;&c_a;&c_c;&c_y;&c_ja; <klr tip="amb">(&c_u;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_a;)</klr></trd>
  <trd lng="br">melestradurezh<klr tip="ind"> (al lec'h)</klr></trd>
  <trdgrp lng="cs">
    <trd>administrace</trd>,
    <trd>spr&aacute;vn&iacute; budova</trd>
  </trdgrp>
