<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: akvati.xml,v 1.17 2019/01/27 14:10:17 revo Exp $">
<kap>
  <rad>akvatint</rad>/o
</kap>
<drv mrk="akvati.0o">
  <kap><tld/>o</kap>
  <snc mrk="akvati.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Gravura <ref tip="super" cel="proced.0o">procedo</ref>
      per acido, imitanta la lavumadon:
      <ekz>
        la kuprogravurado kiel rimedo de pentra&jcirc;oreproduktado tamen balda&ubreve; estis
        anstata&ubreve;ita, &ccirc;ar per invento de <tld/>o kaj mezotinto disponi&gcirc;is teknikoj,
        kiuj ebligis griznuancojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=288883"
              >Kuprogravurado</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="akvati.0o.verko">
    <dif>
      <tld lit="A"/>a&jcirc;o, <ref tip="super" cel="verk.0o.propra">verko</ref>
      farita per <tld/>a procedo:
      <ekz>
        la <tld/>aj gravura&jcirc;oj a&ubreve; simple <tld/>oj aperis en la 18-a jarcento, pro la
        klopodo fare de artistoj por rekrei en la gravura&jcirc;oj la efikon de akvareloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=596959"
              >Akvatinta gravura&jcirc;o</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_v;&c_a;&c_t;&c_y;&c_n;&c_t;&c_a;</trd>
  <trd lng="ca">aiguatinta</trd>
  <trd lng="cs">akvatinta</trd>
  <trdgrp lng="en">
    <trd>aquatint</trd>,
    <trd>acid-washing</trd>
  </trdgrp>
  <trd lng="fr">aquatinte</trd>
  <trdgrp lng="hu">
    <trd>akvatinta</trd>,
    <trd>foltmar&aacute;s</trd>
  </trdgrp>
  <trd lng="it">acquatinta</trd>
  <trd lng="nl">aquatint</trd>
  <trd lng="pt">aquatinta</trd>
  <trd lng="ru">&c_a;&c_k;&c_v;&c_a;&c_t;&c_i;&c_n;&c_t;&c_a;</trd>
  <trd lng="sk">akvatina</trd>
</drv>
</art>
