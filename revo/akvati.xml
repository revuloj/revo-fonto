<vortaro>
<art mrk="$Id$">
<kap>
  <rad>akvatint</rad>/o
</kap>
<drv mrk="akvati.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Procedo de gravurado per acido, imitanta la lavumadon.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_v;&c_a;&c_t;&c_y;&c_n;&c_t;&c_a;</trd>
  <trdgrp lng="en">
    <trd>aquatint</trd>,
    <trd>acid-washing</trd>
  </trdgrp>
  <trd lng="fr">aquatinte</trd>
  <trdgrp lng="hu">
    <trd>akvatinta</trd>,
    <trd>foltmar&aacute;s</trd></trdgrp>
  <trd lng="nl">aquatint</trd>
  <trd lng="pt">aquantinta</trd>
  <trd lng="ru">&c_a;&c_k;&c_v;&c_a;&c_t;&c_i;&c_n;&c_t;&c_a;</trd>
</drv>
</art>
