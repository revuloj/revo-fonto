<drv mrk="manil.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      La <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&ccirc;efurbo</ref>
      de <ref tip="malprt" cel="filipin.0oj">Filipinoj</ref>:
      <ekz>
        en <tld/>o neniu venis por ricevi la kargon
        <fnt>
       <aut>HORI Jasuo/pg</aut>
          <vrk><url ref="&Monato;/2000/006893.php">Rubo-ruzo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/11, p. 14</lok>
        </fnt>;
      </ekz>
      <ekz>
        la konstanta arbitracia Kortumo de Hago devas decidi pri la konflikto inter
        <tld/>o kaj Pekino en la Sud&ccirc;ina Maro
        <fnt>
          <aut>Didier Cormorand </aut>
          <vrk><url ref="&Mondediplo;article2372.html">Kaj por kelkaj pliaj rokoj...</url></vrk>
          <lok>Le Monde diplomatique en Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a Golfo
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Manilo">Manilo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_M;&c_a;&c_n;&c_ib;&c_l;&c_a; <klr tip="amb">(&c_g;&c_o;&c_r;&c_a;&c_d;)</klr></trd>
    <trd lng="de">Manila</trd>
    <trd lng="en">Manila</trd>
    <trd lng="fr">Manille</trd>
    <trd lng="nl">Manila</trd>
    <trd lng="pl">Manila</trd>     
    <trd lng="ro">Manila</trd>
    <trd lng="uk">&#x41C;&c_a;&#x43D;&c_ib;&#x43B;&c_a; (&c_s;&c_t;&#x43E;&#x43B;&c_i;&c_c;&#x44F;)</trd>
  </snc>  
  <trd lng="ja">&#x30DE;&#x30CB;&#x30E9;  [<ind>&#x307E;&#x306B;&#x3089;</ind>]</trd>
</drv>
