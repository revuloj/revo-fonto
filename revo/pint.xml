<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pint</rad>/o
</kap>
<drv mrk="pint.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pint.0o.akuto">
    <dif>
      Akuta kaj pika ekstremo:
      <ekz>
        <tld/>o de glavo, kudrilo, cirkelo.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_a;&c_s;&c_t;&c_r;&c_y;&c_jo;</trd>,
      <trd>&c_d;&c_zh;&c_a;&c_l;&c_a;</trd>,
      <trd>&c_v;&c_o;&c_s;&c_t;&c_r;&c_y; &c_k;&c_a;&c_n;&c_je;&c_c;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_n;&c_ch;&c_i;&c_k;</trd>,
      <trd>&c_o;&c_s;&c_t;&c_r;&c_y;&c_j; &c_k;&c_o;&c_n;&c_je;&c_c;</trd>,
      <trd>&c_o;&c_s;&c_t;&c_r;&c_i;&c_jo;</trd>
    </trdgrp>
  </snc>
  <snc mrk="pint.0o.ekstremo">
    <dif>
      Ekstremo:
      <ekz>
        <tld/>o de la sonorilturo;
      </ekz>
      <ekz>
        urbestro, eniras tre malla&ubreve;te per la <tld/>oj de la piedoj
        <fnt><bib>Revizoro</bib>, <lok>akto 3a, sceno 9a</lok></fnt>;
      </ekz>
      <ekz>
        li vidas nur &gcirc;is la <tld/>o de sia nazo
        <klr>(estas malsagaca, ne anta&ubreve;videma)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>mam<tld/>o</ind>;
      </ekz>
      <ekz>
        <tld/>o de monto;
      </ekz>
      <ekz>
        ter<tld/>o
        <klr>(promontoro)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en la <tld/>o <klr>(supro)</klr> de la tutmonda intereso
        staras tiu demando;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        atingi la <tld/>ojn de la homa penso;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        super<tld/>e <klr>(supra&jcirc;e)</klr>, nur tu&scirc;inte
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kulmin.0o">kulmino</ref>,
      <ref cel="supr.0o">supro</ref>,
      <ref cel="vert.0o">verto</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_v;&c_ja;&c_r;&c_h;&c_u;&c_sh;&c_k;&c_a;</trd>,
      <trd>&c_v;&c_ja;&c_r;&c_sh;&c_y;&c_n;&c_ja;</trd>,
      <trd>&c_p;&c_ib;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_je;&c_r;&c_h;&c_u;&c_sh;&c_k;&c_a;</trd>,
      <trd>&c_v;&c_je;&c_r;&c_sh;&c_i;&c_n;&c_a;</trd>,
      <trd>&c_p;&c_i;&c_k;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>bout</trd>,
    <trd>pointe</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hegy<klr tip="ind"> (cs&uacute;cs)</klr></trd>,
    <trd>cs&uacute;cs</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>punt</trd>,
    <trd>spits</trd>,
    <trd>top</trd>
  </trdgrp>
</drv>

<drv mrk="pint.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Fini&gcirc;anta per <tld/>o:
      <ekz>
        <tld/>a tran&ccirc;ilo, bastono, &ccirc;apelo, mentono;
      </ekz>
      <ekz>
        monto multe<tld/>a estas la monto Ba&scirc;ana
        <fnt><bib>MT</bib>, <lok>&Psa; 68:15</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_o;&c_s;&c_t;&c_r;&c_a;&c_k;&c_a;&c_n;&c_c;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_s;&c_mol;&c_p;&c_ib;&c_ch;&c_a;&c_s;&c_t;&c_y;</trd>
  </trdgrp>
  <trd lng="fr">pointu</trd>
  <trdgrp lng="hu">
    <trd>hegyes</trd>,
    <trd>cs&uacute;csos</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_s;&c_t;&c_r;&c_o;&c_k;&c_o;&c_n;&c_je;&c_ch;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="pint.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>oforma:
      <ekz>
        al la &ccirc;ielo <tld/>as du sonorilturoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">pointer<klr tip="amb"> (&ecirc;tre en forme de
  pointe)</klr></trd>
  <trd lng="hu">cs&uacute;csosodik</trd>
</drv>

<drv mrk="pint.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Igi <tld/>a:
      <ekz>
        <tld/>igi krajonon;
      </ekz>
      <ekz>
        malbonhumore <tld/>igi la bu&scirc;on
        <klr>(fari lipgrimacon)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akr.0a">akra</ref>,
      <ref cel="kojn">kojna</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_v;&c_a;&c_s;&c_t;&c_r;&c_y;&c_c;&c_mol;</trd>
  <trdgrp lng="fr">
    <trd>tailler en pointe</trd>
  </trdgrp>
  <trd lng="hu">kihegyez</trd>
  <trdgrp lng="nl">
    <trd>spitsen</trd>,
    <trd>puntig maken</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_o;&c_s;&c_t;&c_r;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="pint.sen0igi">
  <kap>sen<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malaperigi la <tld/>on de io:
      <ekz>
        sen<tld/>igi plumon, glavon;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sen<tld/>igi <klr>(senefikigi)</klr> argumenton, aserton.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_t;&c_u;&c_p;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="fr">
    <trd>&eacute;mousser la pointe</trd>,
    <trd>&eacute;pointer</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_t;&c_u;&c_p;&c_i;&c_t;&c_mol;</trd>
</drv>
</art>
