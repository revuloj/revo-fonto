<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pint</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pint.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pint.0o.akuto">
    <dif>
      Akuta kaj pika ekstremo:
      <ekz>
        <tld/>o de glavo, kudrilo, cirkelo.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_a;&c_s;&c_t;&c_r;&c_y;&c_jo;</trd>,
      <trd>&c_d;&c_zh;&c_a;&c_l;&c_a;</trd>,
      <trd>&c_v;&c_o;&c_s;&c_t;&c_r;&c_y; &c_k;&c_a;&c_n;&c_je;&c_c;</trd>
    </trdgrp>
    <trd lng="de">Spitze</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_n;&c_ch;&c_i;&c_k;</trd>,
      <trd>&c_o;&c_s;&c_t;&c_r;&c_y;&c_j; &c_k;&c_o;&c_n;&c_je;&c_c;</trd>,
      <trd>&c_o;&c_s;&c_t;&c_r;&c_i;&c_jo;</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>punta</trd>,
      <trd>puya</trd>
    </trdgrp>
  </snc>
  <snc mrk="pint.0o.ekstremo">
    <dif>
      Ekstremo:
      <ekz>
        <tld/>o de la sonorilturo;
      </ekz>
      <ekz>
        urbestro, eniras tre malla&ubreve;te per la <tld/>oj de la piedoj
        <fnt><bib>Revizoro</bib>, <lok>akto 3a, sceno 9a</lok></fnt>;
      </ekz>
      <ekz>
        li vidas nur &gcirc;is la <tld/>o de sia nazo
        <klr>(estas malsagaca, ne anta&ubreve;videma)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>o de monto;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en la <tld/>o <klr>(supro)</klr> de la tutmonda intereso
        staras tiu demando;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        atingi la <tld/>ojn de la homa penso;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        super<tld/>e <klr>(supra&jcirc;e)</klr>, nur tu&scirc;inte
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kulmin.0o">kulmino</ref>,
      <ref cel="supr.0o">supro</ref>,
      <ref cel="vert.0o">verto</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_v;&c_ja;&c_r;&c_h;&c_u;&c_sh;&c_k;&c_a;</trd>,
      <trd>&c_v;&c_ja;&c_r;&c_sh;&c_y;&c_n;&c_ja;</trd>,
      <trd>&c_p;&c_ib;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>&auml;u&szlig;erstes Ende</trd>,
      <trd>Spitze</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>punta</trd>,
      <trd>extremo</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_je;&c_r;&c_h;&c_u;&c_sh;&c_k;&c_a;</trd>,
      <trd>&c_v;&c_je;&c_r;&c_sh;&c_i;&c_n;&c_a;</trd>,
      <trd>&c_p;&c_i;&c_k;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>punta</trd>,
    <trd>cap</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bout</trd>,
    <trd>pointe</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hegy<klr tip="ind"> (cs&uacute;cs)</klr></trd>,
    <trd>cs&uacute;cs</trd>
  </trdgrp>
  <trd lng="it">punta</trd>
  <trdgrp lng="nl">
    <trd>punt</trd>,
    <trd>spits</trd>,
    <trd>top</trd>
  </trdgrp>
  <trdgrp lng="oc">
    <trd>poncha</trd>,
    <trd>cap</trd>
  </trdgrp>
  <trd lng="pt">ponta</trd>
</drv>

<drv mrk="pint.0a">
  <kap><tld/>a</kap>
  <snc mrk="pint.0a.akuta">
    <dif>
      Fini&gcirc;anta per <tld/>o:
      <ekz>
        <tld/>a tran&ccirc;ilo, bastono, &ccirc;apelo, mentono;
      </ekz>
      <ekz>
        monto multe<tld/>a estas la monto Ba&scirc;ana
        <fnt><bib>MT</bib>, <lok>&Psa; 68:15</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_o;&c_s;&c_t;&c_r;&c_a;&c_k;&c_a;&c_n;&c_c;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_s;&c_mol;&c_p;&c_ib;&c_ch;&c_a;&c_s;&c_t;&c_y;</trd>
  </trdgrp>
  <trd lng="de">spitz</trd>
  <trd lng="fr">pointu</trd>
  <trdgrp lng="es">
    <trd>puntiagudo, -a</trd>,
    <trd>puntudo, -a</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hegyes</trd>,
    <trd>cs&uacute;csos</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>a <ind>punta</ind></trd>,
    <trd>appuntito</trd>,
    <trd>puntuto</trd>,
    <trd>aguzzo</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_s;&c_t;&c_r;&c_o;&c_k;&c_o;&c_n;&c_je;&c_ch;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="pint.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="pint.0i.STAT">
    <dif>
      Esti <tld/>oforma:
      <ekz>
        al la &ccirc;ielo <tld/>as du sonorilturoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">pointer<klr tip="amb"> (&ecirc;tre en forme de
  pointe)</klr></trd>
  <trd lng="hu">cs&uacute;csosodik</trd>
  <trd lng="it">essere a <ind>punta</ind></trd>
</drv>

<drv mrk="pint.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pint.0igi.KOMUNE">
    <dif>
      Igi <tld/>a:
      <ekz>
        <tld/>igi krajonon;
      </ekz>
      <ekz>
        malbonhumore <tld/>igi la bu&scirc;on
        <klr>(fari lipgrimacon)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akr.0a">akra</ref>,
      <ref cel="kojn">kojna</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_v;&c_a;&c_s;&c_t;&c_r;&c_y;&c_c;&c_mol;</trd>
  <trd lng="de">anspitzen</trd>
  <trd lng="fr">tailler en pointe</trd>
  <trd lng="hu">kihegyez</trd>
  <trdgrp lng="it">
    <trd>fare la <ind>punta</ind></trd>,
    <trd>appuntire</trd>
  </trdgrp>
  <trd lng="es">sacarle punta<klr> (a algo)</klr></trd>
  <trdgrp lng="nl">
    <trd>spitsen</trd>,
    <trd>puntig maken</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_o;&c_s;&c_t;&c_r;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="pint.0umo">
  <kap><tld/>umo</kap>
  <snc mrk="pint.0umo.sprito">
    <dif>
      Mallonga, ofte rakontofina <ref tip="vid" 
      cel="sprit.0ajxo">sprita&jcirc;o</ref>, kiu postulas bonan 
      komprenon:
      <ekz>
        &ccirc;u Avinjo Byrne kaptis la <tld/>umon?
        <fnt>
          <aut>M. Binchy, trad. W. Kirschstein</aut>,
          <vrk><url ref="http://www.esperanto.org/Ondo/Tl-ang04.htm">
          La plej bela gastejo de la urbo</url></vrk>,
          <bib>LOdE</bib>,
          <lok>2001:1 (75)</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Zuspitzung</trd>,
      <trd>&Uuml;berspitzung</trd>
    </trdgrp>
    <trd lng="fr">pointe<klr> (d'humour)</klr></trd>
    <trd lng="it">stoccata<klr> (umoristica)</klr></trd>
  </snc>
</drv>

<drv mrk="pint.bor0o">
  <kap>bor<tld/>o</kap>
  <snc mrk="pint.bor0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <tld lit="P"/>a tran&ccirc;randa ilo por bori;
      <ref tip="sin" cel="bor.0ilo.TEK">borilo<sncref/></ref>:
      <ekz>
        mi a&ccirc;etis aron da bor<tld/>oj specialaj por bori 
        lignon
        <fnt>
          <aut>D. Rutan</aut>,
          <vrk><url
          ref="http://ligneroj.wordpress.com/2014/10/28/fakaro-por-pikborpintoj/">
          Fakaro por Pikborpintoj</url></vrk>,
          <lok>Ligneroj, 2014-10-28</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_v;&c_je;&c_r;&c_d;&c_z;&c_je;&c_l;</trd>
  <trdgrp lng="ca">
    <trd>trepant</trd>,
    <trd>barrina</trd>,
    <trd>broca</trd>
  </trdgrp>
  <trd lng="de">Bohrer</trd>
  <trdgrp lng="fr">
    <trd>foret</trd>,
    <trd>m&egrave;che<klr tip="ind"> (foret)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>barrena</trd>,
    <trd>barreno</trd>,
    <trd>broca</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wiert&lstroke;o</trd>,
    <trd>&sacute;wider</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_u;&c_r;&c_a;&c_v;</trd>,
    <trd>&c_s;&c_v;&c_je;&c_r;&c_l;&c_o;</trd>
  </trdgrp>
  <trd lng="sv">borr</trd>
</drv>

<drv mrk="pint.mam0o">
  <kap>mam<tld/>o</kap>
  <snc mrk="pint.mam0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Elstara&jcirc;o &ccirc;e la <ref tip="malprt" cel="mam.0o.ina">mamo</ref> 
      de homoj kaj aliaj <ref cel="mam.0uloj">mamuloj</ref>, tra kiu &ccirc;e
      inoj 
      la idoj su&ccirc;as lakton: 
      <ekz>
        la trabluze videblaj mam<tld/>oj de Helga
        <fnt><aut>Pejno S.</aut>:
        <vrk>La lernemulo</vrk>,
        <bib>Monato</bib><lok>jaro 1999a, numero 6a, p. 23a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="cic.0o">cico</ref>,
      <ref cel="mam.0eto">mameto</ref>
    </refgrp>
  </snc>
  <trd lng="br">beg ar vronn</trd>
  <trd lng="ca">mugr&oacute;</trd>
  <trd lng="de">Brustwarze</trd>
  <trd lng="en">nipple</trd>
  <trdgrp lng="fr">
    <trd>mamelon</trd>,
    <trd>t&ecirc;ton</trd>
  </trdgrp>
  <trd lng="he">&pe;&tet;&mem;&he;</trd>
  <trd lng="it">capezzolo</trd>
  <trd lng="hu">mellbimb&oacute;</trd>
  <trd lng="lat">papilla, ae</trd>
  <trd lng="nl">tepel</trd>
  <trd lng="pl">brodawka</trd>
  <trd lng="pt">mamilo</trd>
  <trd lng="ru">&c_s;&c_o;&c_s;&c_o;&c_k;</trd>
  <trd lng="sv">br&ouml;stv&aring;rta</trd>
  <trd lng="tr">meme ba&scedil;&inodot;</trd>
</drv>

<drv mrk="pint.pied0o">
  <kap>pied<tld/>o</kap>
  <snc mrk="pint.pied0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Ekstremo de <ref tip="malprt" cel="pied.0o.ANA">piedo</ref>, la 
      fingroj a&ubreve; ekstremaj artikoj de piedfingroj:
      <ekz>
        la scivola edzino elliti&gcirc;as, pied<tld/>e iras al la kuirejo kaj
        
        diskrete enrigardas
        <fnt><aut>L. Beaucaire</aut>, <vrk>Kruko kaj Baniko el 
        Bervalo</vrk>, <lok>1974</lok></fnt>;
      </ekz>
      <ekz>
        sepjara &ccirc;ifonulo ekstarante sur la pied<tld/>ojn
        <klr>...</klr>
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/ora.html">
          La ora &ccirc;eno</url></vrk>,
          <lok>2009</lok>
        </fnt>.  
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Zehenspitze</trd>
  <trd lng="fr">pointe des pieds</trd>
  <trd lng="it"><ind>punta</ind> del piede</trd>
</drv>

<drv mrk="pint.sen0igi">
  <kap>sen<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pint.sen0igi.KOMUNE">
    <dif>
      Malaperigi la <tld/>on de io:
      <ekz>
        sen<tld/>igi plumon, glavon;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sen<tld/>igi <klr>(senefikigi)</klr> argumenton, aserton.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_t;&c_u;&c_p;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="fr">
    <trd>&eacute;mousser la pointe</trd>,
    <trd>&eacute;pointer</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>kappen</trd>,
    <trd>kupieren</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>quitarle la punta</trd>,
    <trd>gastarle la punta</trd>
  </trdgrp>
  <trd lng="it">spuntare</trd>
  <trd lng="ru">&c_z;&c_a;&c_t;&c_u;&c_p;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="pint.ter0o">
  <kap>ter<tld/>o</kap>
  <snc mrk="pint.ter0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <ref tip="dif" cel="promon.0o.GEOG">promontoro</ref>
  </snc>
  <trdgrp lng="fr">
    <trd>cap</trd>,
    <trd>promontoire</trd>
  </trdgrp>
  <trd lng="it">promontorio</trd>
</drv>
</art>
