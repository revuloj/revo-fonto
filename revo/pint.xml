<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pint.xml,v 1.47 2024/07/04 09:30:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pint</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pint.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pint.0o.akuto">
    <dif>
      <ref tip="vid" cel="akut.0a.angulo">Akuta</ref> kaj
      <ref tip="vid" cel="pik.0a">pika</ref>
      ekstremo:
      <ekz>
        <tld/>o de glavo, kudrilo, cirkelo.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_a;&c_s;&c_t;&c_r;&c_y;&c_jo;</trd>,
      <trd>&c_d;&c_zh;&c_a;&c_l;&c_a;</trd>,
      <trd>&c_v;&c_o;&c_s;&c_t;&c_r;&c_y;
      &c_k;&c_a;&c_n;&c_je;&c_c;</trd>
    </trdgrp>
    <trd lng="de">Spitze</trd>
    <trd lng="en">point</trd>
    <trdgrp lng="es">
      <trd>punta</trd>,
      <trd>puya</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>szpic</trd>,
      <trd>czubek</trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>punct</trd>,
      <trd>deget</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_n;&c_ch;&c_i;&c_k;</trd>,
      <trd>&c_o;&c_s;&c_t;&c_r;&c_y;&c_j;
      &c_k;&c_o;&c_n;&c_je;&c_c;</trd>,
      <trd>&c_o;&c_s;&c_t;&c_r;&c_i;&c_jo;</trd>
    </trdgrp>
  </snc>
  <snc mrk="pint.0o.ekstremo">
    <dif>
      <ref tip="lst" lst="voko:formoj" cel="ekstre.0o.ajxo">Ekstremo<sncref/></ref>:
      <ekz>
        <tld/>o de la sonorilturo;
      </ekz>
      <ekz>
        urbestro, eniras tre malla&ubreve;te per la <tld/>oj de la
        piedoj
        <fnt><bib>Revizoro</bib>, <lok>akto 3a, sceno 9a</lok></fnt>;
      </ekz>
      <ekz>
        li vidas nur &gcirc;is la <tld/>o de sia nazo
        <klr>(estas malsagaca, ne anta&ubreve;videma)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>o de monto;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en la <tld/>o <klr>(supro)</klr> de la tutmonda intereso
        staras tiu demando;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        atingi la <tld/>ojn de la homa penso;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        super<tld/>e <klr>(supra&jcirc;e)</klr>, nur tu&scirc;inte
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kulmin.0o">kulmino</ref>,
      <ref cel="supr.0o">supro</ref>,
      <ref cel="vert.0o">verto</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_v;&c_ja;&c_r;&c_h;&c_u;&c_sh;&c_k;&c_a;</trd>,
      <trd>&c_v;&c_ja;&c_r;&c_sh;&c_y;&c_n;&c_ja;</trd>,
      <trd>&c_p;&c_ib;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>&auml;u&szlig;erstes Ende</trd>,
      <trd>Spitze</trd>
    </trdgrp>
    <trd lng="en">tip</trd>
    <trdgrp lng="es">
      <trd>punta</trd>,
      <trd>extremo</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>szczyt</trd>,
      <trd>wierzcho&lstroke;ek</trd>,
      <trd>czubek</trd>,
      <trd>iglica</trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>v&acirc;rf</trd>,
      <trd>culme</trd>,
      <trd>turl&abreve;</trd>,
      <trd>top</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_je;&c_r;&c_h;&c_u;&c_sh;&c_k;&c_a;</trd>,
      <trd>&c_v;&c_je;&c_r;&c_sh;&c_i;&c_n;&c_a;</trd>,
      <trd>&c_p;&c_i;&c_k;</trd>
    </trdgrp>
  </snc>
  <rim>
    En &leftquot;La Interrompita Kanto&rightquot; Kabe uzas la vorton anka&ubreve; sinonime kun
    <ref tip="sin" cel="punt.0o">punto</ref>, supozeble influita de la germana.
  </rim>
  <trd lng="bg">&c_v;&c_r;&c_malmol;&c_h;</trd>
  <trd lng="bo">&#xF62;&#xFA9;&#xF7A;&#xF0B;</trd>
  <trdgrp lng="ca">
    <trd>punta</trd>,
    <trd>cap</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hrot</trd>,
    <trd>vrchol</trd>,
    <trd>&scaron;pice</trd>,
    <trd>&scaron;pi&ccaron;ka</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bout</trd>,
    <trd>pointe</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hegy<klr tip="ind"> (cs&uacute;cs)</klr></trd>,
    <trd>cs&uacute;cs</trd>
  </trdgrp>
  <trd lng="it">punta</trd>
  <trdgrp lng="ja">
    <trd>&#20808;  <pr>&#x305B;&#12435;</pr></trd>,
    <trd>&#20808;&#x7AEF;  <pr>&#x305B;&#12435;&#x305F;&#12435;</pr></trd>,
    <trd>&#38914;&#x4E0A;  <pr>&#12385;&#12423;&#12358;&#12376;&#12423;&#12358;</pr></trd>,
    <trd>&#38914;&#x70B9;  <pr>&#12385;&#12423;&#12358;&#12390;&#12435;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>punt</trd>,
    <trd>spits</trd>,
    <trd>top</trd>
  </trdgrp>
  <trdgrp lng="oc">
    <trd>poncha</trd>,
    <trd>cap</trd>
  </trdgrp>
  <trd lng="pt">ponta</trd>
  <trdgrp lng="sk">
    <trd>hrot</trd>,
    <trd>vrchol</trd>,
    <trd>&scaron;pi&ccaron;ka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_ib;&#x43D;&c_ch;&c_i;&#x43A;</trd>,
    <trd>&c_g;&#x43E;&c_s;&c_t;&c_r;&c_i;&c_j; &#x43A;&c_ib;&#x43D;&c_je;&c_c;&#x44C;</trd>,
    <trd>&c_v;&c_ib;&c_s;&c_t;&c_r;&#x44F;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_h;&c_ib;&c_v;&#x43A;&c_a;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_sh;&c_i;&#x43D;&c_a;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_sh;&#x43E;&#x43A;</trd>,
    <trd>&#x43F;&c_ib;&#x43A;</trd>,
    <trd>&c_sh;&#x43F;&c_i;&#x43B;&#x44C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5C16; <pr>ji&amacron;n</pr></trd>,
    <trd>&#x68A2; <pr>sh&amacron;o</pr></trd>,
    <trd>&#x676A; <pr>mi&#x1CE;o</pr></trd>,
    <trd>&#26497;&#x81F4; <pr>j&#xED;zh&#xEC;</pr></trd>,
    <trd>&#26997;&#x81F4; <pr>j&#xED;zh&#xEC;</pr></trd>,
    <trd>&#x92D2; <pr>f&emacron;ng</pr></trd>,
    <trd>&#x950B; <pr>f&emacron;ng</pr></trd>,
    <trd>&#x939E; <pr>b&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pint.0a">
  <kap><tld/>a</kap>
  <snc mrk="pint.0a.akuta">
    <dif>
      Fini&gcirc;anta per <tld/>o:
      <ekz>
        <tld/>a tran&ccirc;ilo, bastono, &ccirc;apelo, mentono;
      </ekz>
      <ekz>
        monto multe<tld/>a estas la monto Ba&scirc;ana
        <fnt><bib>MT</bib>, <lok>&Psa; 68:15</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_o;&c_s;&c_t;&c_r;&c_a;&c_k;&c_a;&c_n;&c_c;&c_o;&c_v;&c_y;</trd>,
      <trd>&c_s;&c_mol;&c_p;&c_ib;&c_ch;&c_a;&c_s;&c_t;&c_y;</trd>
    </trdgrp>
    <trd lng="cs">&scaron;pi&ccaron;at&yacute;</trd>
    <trd lng="de">spitz</trd>
    <trdgrp lng="en">
      <trd>pointed</trd>,
      <trd>spiked</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>puntiagudo, -a</trd>,
      <trd>puntudo, -a</trd>
    </trdgrp>
    <trd lng="fr">pointu</trd>
    <trdgrp lng="hu">
      <trd>hegyes</trd>,
      <trd>cs&uacute;csos</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>a <ind>punta</ind></trd>,
      <trd>appuntito</trd>,
      <trd>puntuto</trd>,
      <trd>aguzzo</trd>
    </trdgrp>
    <trd lng="pl">szpiczasty</trd>
    <trd lng="ro">v&icirc;rf</trd>
    <trd lng="ru">&c_o;&c_s;&c_t;&c_r;&c_o;&c_k;&c_o;&c_n;&c_je;&c_ch;&c_n;&c_y;&c_j;</trd>
    <trd lng="sk">&scaron;picat&yacute;</trd>
    <trdgrp lng="zh">
    <trd>&#x5C16;&#x5C16; <pr>ji&amacron;nji&amacron;n</pr></trd>,
    <trd>&#x5C16;&#x92B3; <pr>ji&amacron;nru&#xEC;</pr></trd>,
    <trd>&#x5C16;&#38160; <pr>ji&amacron;nru&#xEC;</pr></trd>,
    <trd>&#21073;&#x5C16; <pr>ji&#xE0;nji&amacron;n</pr></trd>,
    <trd>&#x528D;&#x5C16; <pr>ji&#xE0;nji&amacron;n</pr></trd>,
    <trd>&#x5C16;&#x7A84; <pr>ji&amacron;nzh&#x1CE;i</pr></trd>,
    <trd>&#x5C16; <pr>ji&amacron;n</pr></trd>,
    <trd>&#x93C3; <pr>z&#xFA;</pr></trd>,
    <trd>&#x955E; <pr>z&#xFA;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="pint.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Gravula, eminenta, plej fama:
      <ekz>
        Ma&lstroke;gorzata Handzlik renkontis la <tld/>an respondeculon
        pri
        kulturaj aferoj en EU, la komisionanon J&aacute;n Figel'
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.liberafolio.org/2005/Agado/figelrendevuo">
          Ma&lstroke;gorzata Handzlik renkontis pintan
          EU-estron</url></vrk>,
          <lok>libera folio, 2006-01-21</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_ja;&c_r;&c_h;&c_o;&c_w;&c_n;&c_y;</trd>,
      <trd>&c_t;&c_o;&c_p;-</trd>
    </trdgrp>
    <trd lng="en">top</trd>
    <trdgrp lng="fr">
      <trd>au sommet</trd>,
      <trd>de <ind>pointe</ind></trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#26497; <pr>j&#xED;</pr></trd>,
    <trd>&#26997; <pr>j&#xED;</pr></trd>,
    <trd>&#38914;&#x4E0A; <pr>d&#x1D0;ngsh&#xE0;ng</pr></trd>,
    <trd>&#39030;&#x4E0A; <pr>d&#x1D0;ngsh&#xE0;ng</pr></trd>,
    <trd>&#38914;&#x5C16;&#x7D1A; <pr>d&#x1D0;ngji&amacron;nj&#xED;</pr></trd>,
    <trd>&#39030;&#x5C16;&#x7EA7; <pr>d&#x1D0;ngji&amacron;nj&#xED;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="ja">
    <trd>&#20808;&#x7AEF;&#x306E;  <pr>&#x305B;&#12435;&#x305F;&#12435;&#x306E;</pr></trd>,
    <trd>&#38914;&#x70B9;&#x306E;  <pr>&#12385;&#12423;&#12358;&#12390;&#12435;&#x306E;</pr></trd>,
    <trd>&#20808;&#x306E;&#12392;&#x304C;&#12387;&#x305F;  <pr>&#12373;&#x304D;&#x306E;&#12392;&#x304C;&#12387;&#x305F;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&#x43E;&c_s;&c_t;&c_r;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_g;&#x43E;&c_s;&c_t;&c_r;&c_je;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="pint.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="pint.0i.STAT">
    <dif>
      Elmontri <tld/>on; <ref tip="sin" cel="kulmin.0i.KOMUNE">kulmini<sncref/></ref>:
      <ekz>
        al la &ccirc;ielo <tld/>as du sonorilturoj;
      </ekz>
      <ekz>
        la plej rapidaj procesoroj <tld/>is &ccirc;e 200 MHz
        anta&ubreve; nur 4 jaroj
        <fnt>

          <aut>Franck Arnaud</aut>
          <vrk><url ref="&Monato;/2001/007114.html">Intel
          alfrontas konkurencon</url></vrk>
          <bib>Monato</bib>
          <lok>2000/10, p. 16</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_v;&c_o;&c_s;&c_t;&c_r;&c_y;&c_m;</ind></trd>,
      <trd>&c_v;&c_y;&c_s;&c_t;&c_u;&c_p;&c_a;&c_c;&c_mol; <klr>(&c_a;&c_b; &c_n;&c_je;&c_ch;&c_y;&c_m; &c_v;&c_o;&c_s;&c_t;&c_r;&c_y;&c_m;)</klr></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>point</trd>,
      <trd>peak</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Provizi per pinto, <ref tip="sin" cel="pint.0igi">pintigi</ref>:
      <ekz>
        alte supre de la strange <tld/>itaj tegmentoj iras &gcirc;is la
        mezo de la strato
        tegmentaj defluiloj en la formo de drakoj kaj hundoj
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
    </dif>
  <trd lng="be">&c_v;&c_a;&c_s;&c_t;&c_r;&c_y;&c_c;&c_mol;</trd>
  </snc>
  <trd lng="cs">vy&ccaron;n&iacute;vat &scaron;pi&ccaron;kou</trd>
  <trd lng="de">herausspie&szlig;en</trd>
  <trd lng="fr">pointer<klr tip="amb"> (&ecirc;tre en forme de
  pointe)</klr></trd>
  <trd lng="hu">cs&uacute;csosodik</trd>
  <trd lng="it">essere a <ind>punta</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x7A81;&#x304D;&#x51FA;&#x308B;  <pr>&#12388;&#x304D;&#12391;&#x308B;</pr></trd>,
    <trd>&#x7A81;&#12387;&#x7ACB;&#12388;  <pr>&#12388;&#12387;&#x305F;&#12388;</pr></trd>
  </trdgrp>
  <trd lng="pl">wystawa&cacute; szpiczasto</trd>
  <trd lng="ro">ie&#537;i &icirc;n eviden&#x21B;&abreve;</trd>
  <trd lng="sk">vy&ccaron;nieva&tcaron; &scaron;pi&ccaron;kou</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_t;&c_i; &c_g;&#x43E;&c_s;&c_t;&c_r;&c_i;&#x43C;</trd>,
    <trd>&c_s;&c_t;&c_i;&c_r;&c_ch;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x5C16; <pr>ji&amacron;n</pr></trd>
</drv>

<drv mrk="pint.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pint.0igi.KOMUNE">
    <dif>
      Igi <tld/>a:
      <ekz>
        <tld/>igilo estas instrumento por <tld/>igi bastonoforman
        objekton, ekzemple krajono
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=422393">Pintigilo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        malbonhumore <tld/>igi la bu&scirc;on
        <klr>(fari lipgrimacon)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akr.0a">akra</ref>,
      <ref cel="kojn">kojna</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_s;&c_t;&c_r;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_a;&c_s;&c_t;&c_r;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>naost&rcaron;it</trd>,
    <trd>vyhrocovat</trd>,
    <trd>zahrotit</trd>,
    <trd>za&scaron;pi&ccaron;atit</trd>
  </trdgrp>
  <trd lng="de">anspitzen</trd>
  <trd lng="en">sharpen</trd>
  <trd lng="es">sacarle punta<klr> (a algo)</klr></trd>
  <trd lng="fr">tailler en pointe</trd>
  <trd lng="hu">kihegyez</trd>
  <trdgrp lng="it">
    <trd>fare la <ind>punta</ind></trd>,
    <trd>appuntire</trd>
  </trdgrp>
  <trd lng="ja">&#12392;&#x304C;&#12425;&#x305B;&#x308B;</trd>
  <trdgrp lng="nl">
    <trd>spitsen</trd>,
    <trd>puntig maken</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>temperowa&cacute;</trd>,
    <trd>zaostrza&cacute;</trd>
  </trdgrp>
  <trd lng="ro">ascu&#x21B;i</trd>
  <trd lng="ru">&c_z;&c_a;&c_o;&c_s;&c_t;&c_r;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>ostr&uacute;ha&tcaron;</trd>,
    <trd>zahroti&tcaron;</trd>,
    <trd>zastr&uacute;ha&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_g;&#x43E;&c_s;&c_t;&c_r;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_r;&c_u;&c_g;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x92B3;&#21270; <pr>ru&#xEC;hu&#xE0;</pr></trd>,
    <trd>&#38160;&#21270; <pr>ru&#xEC;hu&#xE0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pint.0umo">
  <kap><tld/>umo</kap>
  <snc mrk="pint.0umo.sprito">
    <dif>
      Mallonga, ofte rakontofina <ref tip="vid" cel="sprit.0ajxo">sprita&jcirc;o</ref>, kiu postulas bonan
      komprenon:
      <ekz>
        &ccirc;u Avinjo Byrne kaptis la <tld/>umon?
        <fnt>
          <aut>M. Binchy, trad. W. Kirschstein</aut>,
          <vrk><url ref="&LOdE;Tl-ang04.htm">
          La plej bela gastejo de la urbo</url></vrk>,
          <bib>LOdE</bib>,
          <lok>2001:1 (75)</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Pointe</trd>,
      <trd>Wendung</trd>,
      <trd>Zuspitzung</trd>,
      <trd>&Uuml;berspitzung</trd>
    </trdgrp>
    <trd lng="fr">pointe<klr> (d'humour)</klr></trd>
    <trd lng="it">stoccata<klr> (umoristica)</klr></trd>
  </snc>
  <trdgrp lng="be">
