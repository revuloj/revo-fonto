<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: klorof.xml,v 1.20 2023/09/01 15:07:14 revo Exp $">
<kap>
  <rad>klorofil</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="klorof.0o">
  <kap><tld/>o</kap>
  <snc mrk="klorof.0o.BOT">
    <uzo tip="fak">KEM</uzo>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Verda
      <ref tip="super" cel="pigment.0o.BIO">pigmento</ref>
      trovebla en la <ref tip="malprt" cel="kloroplast.0o">kloroplastoj</ref>
      de foliaj kaj arbaj &ccirc;eloj, asimilanta karbonon: 
      <ekz>
        multaj algoj estas verdaj sed la plej multo de la
        specioj enhavas krom <tld/>on ceterajn pigmentojn
        ru&gcirc;ajn, bluajn, a&ubreve; flavajn
        <fnt>
          <aut>R. A. Lewin</aut>,
          <vrk><url ref="https://scienca-revuo.info/article/download/1267/1246">
          La Algoj</url></vrk>,
          <lok>Internacia Somera Universitato, 1965</lok>
        </fnt>.
      </ekz>
    </dif>  
    <trd lng="be">&c_h;&c_l;&c_ja;&c_r;&c_a;&c_f;&c_ib;&c_l;</trd>
    <trd lng="bg">&c_h;&c_l;&c_o;&c_r;&c_o;&c_f;&c_i;&c_l;</trd>
    <trd lng="ca">clorofil&middot;la </trd>
    <trd lng="de">Chlorophyll</trd>
    <trd lng="en">chlorophyll</trd>
    <trdgrp lng="fa">
      <trd>&f_k;&a_l;&a_r;&a_w;&a_f;&f_y;&a_l;</trd>,
      <trd>&a_s;&a_b;&a_z;&f_y;&a_n;&a_h;</trd>
    </trdgrp>
    <trd lng="fr">chlorophylle</trd>
    <trdgrp lng="hu">
      <trd>klorofill</trd>,
      <trd>lev&eacute;lz&ouml;ld</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>bladgroen</trd>,
      <trd>chlorofyl</trd>
    </trdgrp>
    <trd lng="pl">chlorofil</trd>
    <trd lng="ro">clorofil&abreve;</trd>
    <trd lng="ru">&c_h;&c_l;&c_o;&c_r;&c_o;&c_f;&c_i;&c_l;&c_l;</trd>
  </snc>
  <trd lng="cs">chlorofyl</trd>
  <trd lng="ja">&#x8449;&#x7DD1;&#x7D20;  [<ind>&#x3088;&#x3046;&#x308A;&#x3087;&#x304F;&#x305D;</ind>]</trd>
  <trdgrp lng="sk">
    <trd>chlorofyl</trd>,
    <trd>listov&aacute; zele&ncaron;</trd>
  </trdgrp>
  <trd lng="uk">&#x445;&#x43B;&#x43E;&#x440;&#x43E;&#x444;&#x456;&#x43B;</trd>
</drv>
</art>
