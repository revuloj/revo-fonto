<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Kulomb</rad>/o
</kap>
<drv mrk="kulomb.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Franclingve: <trd lng="fr"><klr tip="amb">Charles de</klr>
      Coulomb</trd>,
      1736-1806, franca <ref tip="lst" cel="fizik.0isto"
      lst="voko:historiaj_personoj">fizikisto</ref>.
    </dif>
  </snc>
  <trd lng="bg">&c_Sh;&c_a;&c_r;&c_l; &c_d;&c_mol;&c_o; <ind>&c_K;&c_u;&c_l;&c_o;&c_n;</ind></trd>
  <trd lng="ca">Coulomb</trd>
  <trd lng="de">Charles de <ind>Coulomb</ind></trd>
  <trd lng="en">Charles de <ind>Coulomb</ind></trd>
  <trd lng="hu">Charles de <ind>Coulomb</ind></trd>
  <trd lng="it">Charles Augustin de <ind>Coulomb</ind></trd>
  <trd lng="nl">Charles de <ind>Coulomb</ind></trd>
  <trd lng="pt">Charles Augustin de <ind>Coulomb</ind></trd>
  <trd lng="ru">&c_K;&c_u;&c_l;&c_o;&c_n;</trd>
  <trd lng="sv">Charles de <ind>Coulomb</ind></trd>
</drv>
<drv mrk="kulomb.k0o">
  <kap><tld lit="k"/>o</kap>
  <uzo tip="fak">ELE</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <mlg kod="SI">C</mlg>
  <snc>
    <dif>
      <ref tip="lst" cel="unu.mezur0o"
      lst="voko:mezurunuoj">Mezurunuo</ref> de
      <ref cel="sxarg.0o.grando">&scirc;argo</ref> en la Internacia Sistemo
      de unuoj:
      <ekz>
        unu <tld lit="k"/>o egalas al la &scirc;argo transportata dum
        unu <ref cel="sekund.0o.tempo">sekundo</ref> de kurento je
        unu <ref cel="amper.a0o">ampero</ref>;
      </ekz>
      <ekz>
        la &scirc;argo de elektrono egalas al -1,602.10<sup>-19</sup>
        <tld lit="k"/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_k;&c_u;&c_l;&c_o;&c_n;</trd>
  <trd lng="ca">coulomb</trd>
  <trd lng="de">Coulomb</trd>
  <trd lng="en">coulomb</trd>
  <trd lng="es">culombio</trd>
  <trd lng="fr">coulomb</trd>
  <trd lng="hu">coulomb</trd>
  <trd lng="it">coulomb</trd>
  <trd lng="nl">coulomb</trd>
  <trd lng="pt">coulomb</trd>
  <trd lng="ru">&c_k;&c_u;&c_l;&c_o;&c_n;</trd>
  <trd lng="sv">coulomb</trd>
</drv>
</art>
