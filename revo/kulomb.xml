<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Kulomb</rad>/o
</kap>
<drv mrk="kulomb.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Franclingve: Charles de <trd lng="fr">Coulomb</trd>,
      1736-1806. Franca fizikisto.
    </dif>
  </snc>
  <trd lng="de">Charles de <ind>Coulomb</ind></trd>
  <trd lng="en">Charles de <ind>Coulomb</ind></trd>
  <trd lng="hu">Charles de <ind>Coulomb</ind></trd>
  <trd lng="nl">Charles de <ind>Coulomb</ind></trd>
  <trd lng="pt">Charles Augustin de <ind>Coulomb</ind></trd>
  <trd lng="ru">&c_K;&c_u;&c_l;&c_o;&c_n;</trd>
</drv>
<drv mrk="kulomb.k0o">
  <kap><tld lit="k"/>o</kap>
  <uzo tip="fak">ELE</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de
      <ref cel="sxarg.0o.grando">&scirc;argo</ref> en la Internacia Sistemo
      de unuoj; simb. C:
      <ekz>
        unu <tld lit="k"/>o egalas al la &scirc;argo transportata dum
        unu <ref cel="sekund.0o.tempo">sekundo</ref> de kurento je
        unu <ref cel="amper.a0o">ampero</ref>;
      </ekz>
      <ekz>
        la &scirc;argo de elektrono egalas al -1,602.10<sup>-19</sup>
        <tld lit="k"/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Coulomb</trd>
  <trd lng="en">coulomb</trd>
  <trd lng="es">culombio</trd>
  <trd lng="fr">coulomb</trd>
  <trd lng="hu">coulomb</trd>
  <trd lng="nl">coulomb</trd>
  <trd lng="pt">coulomb</trd>
  <trd lng="ru">&c_k;&c_u;&c_l;&c_o;&c_n;</trd>
</drv>
</art>
