<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: geloz.xml,v 1.16 2023/08/31 14:29:52 revo Exp $">
<kap>
  <rad>geloz</rad>/o
</kap>
<drv mrk="geloz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <mlg kod="E">E&#x202F;406</mlg>
  <snc>
    <dif>
      <ref tip="dif" cel="agarag.0o">Agaragaro</ref>:
      <ekz>
        <tld/>o estas ne&ubreve;tralgusta kaj nedigestebla.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>gelosa</trd>,
    <trd>agar-agar</trd>
  </trdgrp>
  <trd lng="cs">geloza</trd>
  <trdgrp lng="de">
    <trd>Japanische Gelatine</trd>,
    <trd>Agar-Agar</trd>
  </trdgrp>
  <trd lng="fr">g&eacute;lose</trd>
  <trd lng="hu">agaragar</trd>
  <trd lng="ja">&#x30B2;&#x30ED;&#x30FC;&#x30B9;</trd>
  <trd lng="nl">agar-agarcultuur</trd>
  <trdgrp lng="pt">
    <trd>&aacute;gar</trd>,
    <trd>&aacute;gar-&aacute;gar</trd>,
    <trd>gelose</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_g;&c_a;&c_r;-&c_a;&c_g;&c_a;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>agar</trd>,
    <trd>&zcaron;el&eacute; z morsk&yacute;ch rias</trd>
  </trdgrp>
  <trd lng="uk">&#x430;&#x433;&#x430;&#x440;-&#x430;&#x433;&#x430;&#x440;</trd>
</drv>
</art>
