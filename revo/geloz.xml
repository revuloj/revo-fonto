<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: geloz.xml,v 1.18 2024/01/08 18:30:07 revo Exp $">
<kap>
  <rad>geloz</rad>/o
  <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="geloz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">KUI</uzo>
  <mlg kod="E">E&#8239;406</mlg>
  <snc>
    <dif>
      <ref tip="dif" cel="agarag.0o">Agaragaro</ref>:
      <ekz>
        <tld/>o estas ne&ubreve;tralgusta kaj nedigestebla;
      </ekz>
      <ekz>
        Svedaj bomboj <klr>[konsistas el]</klr> <klr>[&#8230;]</klr> kakao, tritika faruno,
        sojofaruno, lakta pulvoro, gelozo, sojolecitino, karamela siropo
        <klr>[&#8230;]</klr>
        <fnt>
          <aut>Renate kaj Walter Klag</aut>
          <vrk><url ref="https://www.esperanto-gbg.org/oraklubo/Ora_Jxurnalo_n-ro_51.pdf"
              >Dol&ccirc;aj Svedaj Bomboj</url></vrk>
          <lok>Ora &Jcirc;urnalo, n-ro 51, Majo 2022, p. 15</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>gelosa</trd>,
    <trd>agar-agar</trd>
  </trdgrp>
  <trd lng="cs">geloza</trd>
  <trdgrp lng="de">
    <trd>Japanische Gelatine</trd>,
    <trd>Agar-Agar</trd>
  </trdgrp>
  <trd lng="fr">g&eacute;lose</trd>
  <trd lng="hu">agaragar</trd>
  <trd lng="ja">&#12466;&#12525;&#12540;&#12473;</trd>
  <trd lng="nl">agar-agarcultuur</trd>
  <trdgrp lng="pt">
    <trd>&aacute;gar</trd>,
    <trd>&aacute;gar-&aacute;gar</trd>,
    <trd>gelose</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_g;&c_a;&c_r;-&c_a;&c_g;&c_a;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>agar</trd>,
    <trd>&zcaron;el&eacute; z morsk&yacute;ch rias</trd>
  </trdgrp>
  <trd lng="uk">&c_a;&c_g;&c_a;&c_r;-&c_a;&c_g;&c_a;&c_r;</trd>
  <trd lng="zh">&#30707;&#33457;&#33014; <pr>sh&iacute;hu&amacron;ji&amacron;o</pr></trd>
</drv>
</art>
