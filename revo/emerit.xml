<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: emerit.xml,v 1.24 2022/04/27 14:38:54 revo Exp $">
<kap>
<ofc>8</ofc>
  <rad>emerit</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="emerit.0o">
  <kap><tld/>o</kap>
  <snc mrk="emerit.0o.POL">
    <dif>
      Laboristo, kiu post difinita tempo de servado, ricevas 
      <ref cel="pensi.0o">pension</ref>:
      <ekz>
        mi estas <tld/>o kun malgranda enspezo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato1/monato-9401-telefona-labirinto.txt"
              >Will Green kondukas vin tra sia</url></vrk>
          <lok>94/01</lok>
        </fnt>;
      </ekz>
      <ekz>
        apud lo&gcirc;domoj aperis lokoj por ripozo, benkoj, kie povis sidi kaj <tld/>oj,
        atendantaj siajn nepojn
        <fnt>
          <bib>Monato</bib>
          <aut>Dmitrij Cibulevskij</aut>
          <vrk><url ref="&Monato;/2007/009112.php"
              >Busvoja&gcirc;o</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        la proporcio de aktivaj laboristoj kontra&ubreve; <tld/>oj falis de 3,7 al 3,1
        <fnt>
          <bib>Monato</bib>
          <aut>Alberto Garc&iacute;a Fumero</aut>
          <vrk><url ref="&Monato;/2009/009736.php"
              >Kubo maljuni&gcirc;as</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>a</ind> profesoro;
        <trd lng="ca">jubilat <klr>(adj.)</klr></trd>
        <trdgrp lng="de">
           <trd>in den <ind>Ruhestand versetzt</ind></trd>,
           <trd>emeritiert</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>retrait&eacute;<klr tip="ind"> (adj.)</klr></trd>,
          <trd>en <ind>retraite</ind></trd></trdgrp>
          <trd lng="nl">emeritus<klr tip="ind"> (bn.)</klr></trd> 
        <trd lng="pl">emerytowany</trd>
      </ekz>
      <ekz>
        kiam vi maljuni&gcirc;as, vi ricevas sufi&ccirc;an <tld/>an salajron
        <klr>(vd <ref cel="pensi.0o">pensio</ref>)</klr>
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        vivi <tld/>e
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        li &gcirc;uas sian <ind><tld/>eco</ind>n.
        <trd lng="ca">jubilaci&oacute;</trd>
        <trdgrp lng="de">
           <trd>Ruhestand</trd>,
           <trd>Emeritierung</trd>
        </trdgrp>
        <trd lng="fr">retraite<klr tip="ind"> (&eacute;tat de
retrait&eacute;)</klr></trd>
        <trd lng="nl">emeritaat</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="emerit1.0o">emerituro</ref>,
      <ref cel="pensi.0ulo">pensiulo</ref>,
      <ref cel="vetera.0o">veterano</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_n;&c_s;&c_ib;&c_ja;&c_n;&c_e;&c_r;</trd>
  <trd lng="ca">jubilat <klr>(subst.)</klr></trd>
  <trdgrp lng="cs">
    <trd>d&uring;chodce</trd>,
    <trd>penzista</trd>,
    <trd>vyslou&zcaron;ilec</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ruhest&auml;ndler</trd>,
    <trd>Emeritus</trd>,
    <trd>Rentner</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>retired person</trd>,
    <trd>emeritus</trd>,
    <trd>pensioner</trd>,
    <trd>retiree</trd>,
    <trd>ex-official</trd>
  </trdgrp>
  <trd lng="es">jubilado</trd>
  <trd lng="fr">retrait&eacute;<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="nl">emeritus<klr tip="ind"> (zn.)</klr></trd>
  <trd lng="pl">emeryt</trd>
  <trdgrp lng="pt">
    <trd>aposentado</trd>,
    <trd>reformado</trd>,
    <trd>jubilado</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_n;&c_s;&c_i;&c_o;&c_n;&c_je;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>d&ocirc;chodca</trd>,
    <trd>penzista</trd>,
    <trd>vysl&uacute;&zcaron;ilec</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_je;&c_n;&c_s;&c_ib;&c_o;&c_n;&c_je;&c_r; <klr>(&c_z;&c_a; &c_v;&c_i;&c_s;&c_l;&c_u;&c_g;&c_o;&c_ju; &c_r;&c_o;&c_k;&c_ib;&c_v;)</klr></trd>,
    <trd>&c_v;&c_ib;&c_d;&c_s;&c_t;&c_a;&c_v;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <adm>
    Koherigi kun pensiulo.
    <aut>MB</aut>
  </adm>
</drv>

<drv mrk="emerit.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="emerit.0igxi.POL">
    <dif>
      I&gcirc;i <tld/>o, &ccirc;esigi sian profesian laboron:
      <ekz>
        en 1962 <nom>Waringhien</nom> <tld/>i&gcirc;is kiel honora profesoro kaj en 1963 
        elekti&gcirc;is prezidanto de la Akademio de Esperanto
        <fnt>
          <aut>A. Kor&jcirc;enkov</aut>,
          <vrk><url ref="&LOdE;H-waring.htm">
          Gaston Waringhien</url></vrk>,
          <bib>LOdE</bib>, <lok>2001:6 (80)</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> jam pli frue anoncis al la Estraro sian decidon <tld/>i&gcirc;i la
        16-an de novembro
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        nuntempe en Litovio viroj <tld/>i&gcirc;as je a&gcirc;o de 62,5 jaroj, kaj virinoj je 60
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Straznickas</aut>
          <vrk><url ref="&Steloj;monato2/006806.html"
              >Litovio: &Ccirc;iea &scirc;parado</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kvankam mi emeriti&gcirc;is anta&ubreve; pli ol dudek jaroj, mi ne perdis kontakton kun miaj
        anta&ubreve;aj kolegoj
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2010/009989.php"
              >Voja&gcirc;o unudirekta</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pensi.0uligxi">pensiuli&gcirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_j;&c_s;&c_mol;&c_c;&c_ib; &c_n;&c_a; <ind>&c_p;&c_e;&c_n;&c_s;&c_ib;&c_ju;</ind></trd>
  <trd lng="ca">jubilar-se</trd>
  <trdgrp lng="de">
    <trd>in den <ind>Ruhestand</ind> gehen</trd>,
    <trd>in <ind>Rente</ind> gehen</trd>
  </trdgrp>
  <trd lng="es">jubilar se</trd>
  <trdgrp lng="fr">
    <trd>se retirer<klr> (prendre sa retraite)</klr></trd>,
    <trd>prendre sa <ind>retraite</ind></trd>
  </trdgrp>
</drv>
</art>
