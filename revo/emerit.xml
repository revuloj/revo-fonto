<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>8</ofc>
<rad>emerit</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="emerit.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Oficisto, kiu post difinita tempo de servado, ricevas 
      <ref cel="pensi.0o">pension</ref>:
      <ekz>
        <ind><tld/>a</ind> profesoro;
        <trdgrp lng="de">
           <trd>in den <ind>Ruhest&auml;nd versetzt</ind></trd>,
           <trd>emeritiert</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>retrait&eacute;<klr tip="ind"> (adj.)</klr></trd>,
          <trd>en <ind>retraite</ind></trd></trdgrp> 
      </ekz>
      <ekz>
        <tld/>a salajro<fnt>Z</fnt>,
        pensio;
      </ekz>
      <ekz>
        <tld/>e vivi<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li &gcirc;uas sian <ind><tld/>eco</ind>n.
        <trdgrp lng="de">
           <trd>Ruhestand</trd>,
           <trd>Emeritierung</trd>
        </trdgrp>
        <trd lng="fr">retraite<klr tip="ind"> (&eacute;tat de retrait&eacute;)</klr></trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="emerit1.0o">emerituro</ref>,
      <ref cel="pensi.0ulo">pensiulo</ref>,
      <ref cel="vetera.0o">veterano</ref>.
    </refgrp>
  </snc>
  <adm>
  Cxu vere nur oficistoj povas igxi ~oj?
  Koherigi kun pensiulo.
  <aut>MB</aut>
  </adm>
  <trdgrp lng="de">
     <trd>Ruhest&auml;ndler</trd>,
     <trd>Emeritus</trd>
  </trdgrp>
  <trd lng="fr">retrait&eacute;<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="ru">&c_p;&c_je;&c_n;&c_s;&c_i;&c_o;&c_n;&c_je;&c_r;</trd>
</drv>
</art>
