<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: konsil.xml,v 1.37 2022/11/28 14:28:00 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>konsil</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="konsil.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="konsil.0i.KOMUNE">
    <dif>
      Indiki al iu tion, kion oni opinias, ke li devus fari:
      <ekz>
        la kuracisto <tld/>is al mi iri en &scirc;vitbanejon
        <fnt><bib>F</bib>, <lok>&FE; 40</lok></fnt>;
      </ekz><ekz>
        la montritajn na&ubreve; vortojn mi <tld/>as bone ellerni
        <fnt><bib>F</bib>, <lok>&FE; 30</lok></fnt>;
      </ekz><ekz>
        oni <tld/>as kaj konsolas sed helpi ne
        volas<fnt><bib>PrV</bib></fnt>;
      </ekz><ekz>
        ne<tld/>ebla karaktero<fnt>Z</fnt>
        <klr>(kiu ne volas esti <tld/>ata)</klr>;
      </ekz><ekz>
        nek helpeble, nek <tld/>eble
        <fnt>
          <bib>PrV</bib>
        </fnt>
        <klr>(pp tute embarasita situacio, pri kiu oni povas nek helpi,
        nek <tld/>i)</klr>;
      </ekz><ekz>
        kie ne estas <tld/>eble, tie ne estas helpeble
        <klr>(necesas <tld/>i por helpi)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="sin" cel="rekome.0i.agon">rekomendi</ref>
  <ref tip="super" cel="instig.0i">instigi</ref>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">radit</trd>
  <trd lng="es">aconsejar</trd>
  <trdgrp lng="de">
 <trd>raten</trd>,
 <trd>anraten</trd>,
 <trd>beraten</trd>
  </trdgrp>
  <trdgrp lng="en"><trd>to advise</trd>, <trd>counsel</trd></trdgrp>
  <trd lng="fr">conseiller</trd>
  <trdgrp lng="id">
    <trd>meng<ind>anjur</ind>kan</trd>,
    <trd>me<ind>nasihat</ind>i</trd>
  </trdgrp>
  <trd lng="id"><baz>saran</baz>menyarankan</trd>
  <trd lng="nl">aanraden</trd>
  <trd lng="ms">menasihat</trd>
  <trdgrp lng="pl">
    <trd>radzi&cacute;</trd>,
    <trd>doradza&cacute;</trd>,
    <trd>poleca&cacute;</trd>,
    <trd>konsultowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>aconselhar</trd>,
    <trd>persuadir</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_v;&c_je;&c_t;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">radi&tcaron;</trd>
  <trd lng="cs">radit, poradit</trd>
</drv>

<drv mrk="konsil.0o">
  <kap><tld/>o</kap>
  <snc mrk="konsil.0o.ideo">
    <dif>
      Indiko donita al iu pri tio, kion oni opinias, ke li devus fari:
      <ekz>
        sa&gcirc;ulo a&ubreve;skultas <tld/>on
        <fnt>
          <bib>MT</bib>. <lok>Sentencoj 12:15</lok></fnt>;
      </ekz><ekz>
        Kaj vi de mi akceptu la <tld/>on, //
        Ne amu tro la sunon kaj la stelojn
        <fnt>
          <aut>Goeto, tr. &Z;</aut>:
          <vrk>Ifigenio en Ta&ubreve;rido</vrk>. <lok>3:1</lok>
        </fnt>;
      </ekz><ekz>
        vi ne sekvis la <tld/>on de fidelulo!
        <fnt>
          <aut>Goeto, tr. &Z;</aut>:
          <vrk>Ifigenio en Ta&ubreve;rido</vrk>. <lok>4:2</lok>
        </fnt>;
      </ekz><ekz>
        montroj kaj <tld/>oj estas facilaj
        <fnt><bib>PrV</bib></fnt>;
      </ekz><ekz>
        post dorma trankvilo venas bona <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz><ekz>
        peti <tld/>on de iu
        <fnt>
          <bib>MT</bib>. <lok>Jesaja 40:14</lok></fnt>
        <fnt><bib>Far2</bib>. <lok>&Ccirc;apitro XXIII</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="admon.0o">admono</ref>,
      <ref cel="avert">averto</ref>,
      <ref cel="aviz.0o">avizo</ref>,
      <ref cel="instru.0o">instruo</ref>
    </refgrp>
    <trdgrp lng="de">
    <trd>Rat</trd>,
    <trd>Ratschlag</trd>
    </trdgrp>
    <trd lng="ms">nashihat</trd>
    <trdgrp lng="en">
      <trd>advice</trd>,
      <trd>counsel</trd>, <trd>opinion</trd>
    </trdgrp>
    <trd lng="es">consejo</trd>
      <trdgrp lng="nl">
      <trd>raad</trd>,
      <trd>raadgeving</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>rada</trd>,
      <trd>porada</trd>,
      <trd>konsultacja</trd>
    </trdgrp>
  </snc>
  <snc mrk="konsil.0o.rimedo">
    <uzo tip="stl">ARK</uzo>
    <dif>
      Proponita agmaniero, rimedo:
      <ekz>
        kontra&ubreve; &ccirc;iu malfacilo ekzistas <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz><ekz>
        ni ne scias alian <tld/>on ol prokrasti la aferon;
      </ekz><ekz>
        ne ekzistas sa&gcirc;o, ne ekzistas prudento,
        ne ekzistas <tld/>o kontra&ubreve; la Eternulo
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 21:30</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
    <trd>Empfehlung</trd>,
    <trd>Rat</trd>
    </trdgrp>
    <trd lng="en">counsel</trd>
    <trdgrp lng="nl">
      <trd>raad<klr tip="ind"> (arch.)</klr></trd>,
      <trd>aanbeveling<klr tip="ind"> (arch.)</klr></trd>
    </trdgrp>
    <trd lng="pl">rada</trd>
  </snc>
  <snc mrk="konsil.0o.konsilistaro">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="konsil.0istaro"><tld lit="K"/>istaro</ref>:
      <ekz>
        kunvoki la <tld/>on.
      </ekz>
    </dif>
    <!--
     ne traduku evitindan vorton
    <trdgrp lng="nl">
      <trd>raad<klr tip="ind"> (ontr.)</klr></trd>
    </trdgrp>
    <trd lng="pl">rada</trd>
    -->
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_d;&c_a;</trd>
  <trd
  lng="bo">&#xF56;&#xF66;&#xFB3;&#xF56;&#xF0B;&#xF56;&#xFB1;&#xF0B;&#xF62;&#xF92;&#xFB1;&#xF42;&#xF0B;</trd>
  <trd lng="cs">rada <klr>(ud&ecaron;lovan&aacute;)</klr></trd>
  <trdgrp lng="en">
    <trd>council</trd>,
    <trd>counsel <klr>(legal advocate(s))</klr></trd>
  </trdgrp>
  <trd lng="fr">conseil</trd>
  <trdgrp lng="id">
    <trd>advis</trd>,
    <trd><ind>anjur</ind>an</trd>,
    <trd>nasihat</trd>,
    <trd>saran</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_v;&c_je;&c_t;</trd>
  <trd lng="sk">rada</trd>
</drv>

<drv mrk="konsil.0anaro">
  <kap><tld/>anaro</kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="konsil.0anaro.KOMUNE">
    <ref tip="dif" cel="konsil.0istaro"><tld/>istaro</ref>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_d;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>konsilium</trd>,
    <trd>rada <klr>(sbor)</klr></trd>
  </trdgrp>
  <trd lng="en">council</trd>
  <trd lng="fr">conseil<klr> (groupe de conseillers)</klr></trd>
  <trd lng="id">dewan</trd>
  <trd lng="nl">raad<klr tip="ind"> (raadgevend lichaam;
  ontr.)</klr></trd>
  <trd lng="pl">rada</trd>
  <trd lng="ru">&c_s;&c_o;&c_v;&c_je;&c_t;</trd>
  <trd lng="sk">poradn&yacute; zbor</trd>
</drv>

<drv mrk="konsil.0anto">
  <kap><tld/>anto</kap>
  <snc mrk="konsil.0anto.KOMUNE">
    <dif>
      Tiu, kiu donas konsilojn:
      <ekz>
        li <klr>[&#8230;]</klr> rigardis siajn trankvilanimajn <tld/>antojn
        kun rideto
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz><ekz>
        kortega <tld/>anto
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz><ekz>
        kortegano-<tld/>anto
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz><ekz>
        venko venas per multe da <tld/>antoj  
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 24:6</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_r;&c_a;&c_d;&c_c;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>konzultant</trd>,
    <trd>poradce</trd>,
    <trd>r&aacute;dce</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Berater</trd>,
    <trd>Ratgeber</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>adviser</trd>,
    <trd>advisor</trd>
  </trdgrp>
  <trd lng="es">consejero</trd>
  <trd lng="fr">conseiller</trd>
  <trdgrp lng="id">
    <trd>konselor</trd>,
    <trd>pe<ind>nasihat</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>raadgever</trd>,
    <trd>consulent</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>doradca</trd>,
    <trd>konsultant</trd>
  </trdgrp>
  <trdgrp lng="ru">
  <trd>&c_s;&c_o;&c_v;&c_je;&c_t;&c_n;&c_i;&c_k;</trd>,
  <trd>&c_s;&c_o;&c_v;&c_je;&c_t;&c_ch;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>poradca</trd>,
    <trd>radca</trd>
  </trdgrp>
</drv>

<drv mrk="konsil.0antaro">
  <kap><tld/>antaro</kap>
  <snc mrk="konsil.0antaro.KOMUNE">
    <dif> 
      <ref tip="dif" cel="konsil.0istaro"><tld lit="K"/>istaro</ref>:
      <ekz>
        li povas pli bone klarigi &ccirc;ion al mia sinjoro, &ccirc;ar li estas
        sa&gcirc;ulo kaj membro de la plej alta <tld/>antaro de niaj urboj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz><ekz>
        sidanta en la <tld/>antaro de la sa&gcirc;uloj
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 23:8</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_d;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>konsilium</trd>,
    <trd>rada <klr>(sbor)</klr></trd>
  </trdgrp>
  <trd lng="en">council</trd>
  <trd lng="fr">conseil<klr> (groupe de conseillers)</klr></trd>
  <trd lng="id">dewan</trd>
  <trd lng="nl">raad<klr tip="ind"> (raadgevend
  lichaam)</klr></trd>
  <trd lng="pl">rada <klr>(organ doradczy)</klr></trd>
  <trd lng="ru">&c_s;&c_o;&c_v;&c_je;&c_t;</trd>
  <trd lng="sk">poradn&yacute; zbor</trd>
</drv>

<drv mrk="konsil.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="konsil.0isto.KOMUNE">
    <dif>
      Titolo de &scirc;tata a&ubreve; komunuma oficisto:
      <ekz>
        grandimperiestra noktotabla kantisto kun la rango de <tld/>isto
        de la unua klaso
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz><ekz>
        la jura <tld/>isto <klr>[&#8230;]</klr> havis tre bone en la memoro la
        okazinta&jcirc;ojn de la jaro 1801
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Rat</trd>
    <trd lng="nl">raadsman</trd>
    <trd lng="ru">&c_s;&c_o;&c_v;&c_je;&c_t;&c_n;&c_i;&c_k;</trd>
    <trd lng="es">consejero<klr> (cargo, puesto)</klr></trd>
    <trd lng="pl">radca</trd>
  </snc>
  <snc>
    <dif>
      Persono, kiu profesie donas konsilojn:
      <ekz>
        la re&gcirc;o kunvokis siajn <tld/>istojn, por ke, &ccirc;iu el ili
        eldirante sian opinion, li povu fari decidon
        <fnt>
          <bib>LR</bib>
          <lok>11. Verboj</lok>
        </fnt>;
      </ekz><ekz>
        ni vokos &ccirc;iujn sa&gcirc;ajn <tld/>istojn
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz><ekz>
        li estis la <tld/>isto de la milita ministro
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Berater</trd>
    <trd lng="fr">consultant</trd>
    <trd lng="id">konsultan</trd>
    <trd lng="nl">consulent</trd>
    <trd lng="pl">konsultant</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_o;&c_v;&c_je;&c_t;&c_n;&c_i;&c_k;</trd>,
      <trd>&c_s;&c_o;&c_v;&c_je;&c_t;&c_ch;&c_i;&c_k;</trd>,
      <trd>&c_k;&c_o;&c_n;&c_s;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_n;&c_t;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_r;&c_a;&c_d;&c_c;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_s;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_n;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>konzultant</trd>,
    <trd>poradce</trd>,
    <trd>r&aacute;dce</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>counselor</trd>,
    <trd>adviser</trd>, <trd>advisor</trd>, <trd>consultant</trd>
  </trdgrp>
  <trd lng="fr">conseiller</trd>
  <trdgrp lng="sk">
    <trd>poradca</trd>,
    <trd>radca</trd>
  </trdgrp>
</drv>

<drv mrk="konsil.0istaro">
  <kap><tld/>istaro</kap>
  <snc mrk="konsil.0istaro.KOMUNE">
    <dif>
      Aro da personoj komisiitaj por <tld/>i estron de regno, armeo,
      distrikto ktp:
      <ekz>
        la <tld/>istaro de la Ligo de Nacioj;
      </ekz><ekz>
        la re&gcirc;o kunvokis la <tld/>istaron
        <fnt>
          <bib>ElektFab</bib>
          <lok>La ora birdo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
    <ref cel="komisi1.0o">komisiono</ref>,
    <ref cel="komita.0o">komitato</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_d;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Rat</trd>,
    <trd>Ratsversammlung</trd>
  </trdgrp>
  <trd lng="en">council</trd>
  <trd lng="fr">conseil<klr> (groupe de conseillers)</klr></trd>
  <trd lng="id">dewan</trd>
  <trd lng="nl">raad<klr tip="ind"> (raadgevend lichaam)</klr></trd>
  <trd lng="pl">rada</trd>
  <trd lng="ru">&c_s;&c_o;&c_v;&c_je;&c_t;</trd>
  <trd lng="es">consejo<klr> (grupo de consejeros)</klr></trd>
</drv>

<drv mrk="konsil.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="konsil.0igxi.KOMUNE">
    <dif>
      Peti de iu, ke li <tld/>u, konsulti:
      <ekz>
        la re&gcirc;o <klr>[&#8230;]</klr> <tld/>i&gcirc;is kun siaj servantoj
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 6:8</lok>
        </fnt>;
      </ekz><ekz>
        li <tld/>i&gcirc;is kun siaj eminentuloj kaj fortuloj
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 32:3</lok>
        </fnt>;
      </ekz><ekz>
        mi devas <klr>[&#8230;]</klr> inter<tld/>i&gcirc;i kun miaj servistoj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_s;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>um Rat fragen</trd>,
    <trd>um Rat bitten</trd>,
    <trd>sich <ind>beraten</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>to seek advice</trd>,
    <trd>consult <klr>(with someone)</klr></trd>
  </trdgrp>
  <trd lng="fr">demander conseil</trd>
  <trd lng="id">ber<ind>konsultasi</ind></trd>
  <trd lng="nl">beraadslagen</trd>
  <trdgrp lng="pl">
    <trd>radzi&cacute; si&eogonek;</trd>,
    <trd>konsultowa&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ru">
 <trd>&c_s;&c_o;&c_v;&c_je;&c_t;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
 <trd>&c_k;&c_o;&c_n;&c_s;&c_u;&c_l;&c_mol;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="konsil.de0i">
  <kap>de<tld/>i, <var><kap>mal<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="konsil.de0i.KOMUNE">
    <dif>
      <tld lit="K"/>i iun, ke li ne faru ian agon:
      <ekz>
        kial la prudento devas mal<tld/>i al vi vizitadi nin?
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="avert.0i">averti 3</ref>,
      <ref cel="admon.de0i">deadmoni</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_r;&c_a;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>abraten</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>to advise against</trd>,
    <trd>caution against</trd>
  </trdgrp>
  <trd lng="es">desaconsejar</trd>
