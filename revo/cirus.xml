<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cirus.xml,v 1.25 2022/03/14 15:25:39 revo Exp $">
<kap>
  <rad>cirus</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="cirus.0o">
  <kap><tld/>o</kap>
  <snc mrk="cirus.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Tre maldensa <ref tip="super" cel="nub.0o.MET">nubo</ref>,
      aspektanta kiel plumaro:
      <ekz>
        la nuboj estas malfacile modelizeblaj,
        kaj precipe la proporcioj de <tld/>oj
        <fnt>
          <aut>C. Bertin</aut>,
          <vrk><url ref="http://www.bertin.biz/index.php?titre=allegre-sciencaDubo.htm">
          La rajto pri scienca dubo</url></vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_s;&c_t; &c_o;&c_b;&c_l;&c_a;&c_k;</trd>,
    <trd>&c_c;&c_i;&c_r;&c_u;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>cirrus</trd>,
    <trd>escabellat</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>cirus</trd>,
    <trd>&rcaron;asovit&yacute; oblak</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Federwolke</trd>,
     <trd>Zirruswolke</trd>,
     <trd>Haarlockenwolke</trd>
  </trdgrp>
  <trd lng="el">&psi;&alfa;&lambda;&jota_ton;&delta;&alfa;</trd>
  <trd lng="en">cirrus</trd>
  <trd lng="fa">&a_s;&f_y;&a_r;&a_w;&a_s;<klr tip="ind"> (&a_A;&a_b;&a_r;)</klr></trd>
  <trd lng="fr">cirrus</trd>
  <trd lng="he">&#x5E2;&#x5E0;&#x5E0;&#x5D9;&#x5DD; &#x5E7;&#x5DC;&#x5D9;&#x5DD;</trd>
  <trdgrp lng="hu">
     <trd>pehelyfelh&odblac;</trd>,
     <trd>cirrus</trd>
  </trdgrp>
  <trd lng="it">cirro</trd>
  <trd lng="nl">cirruswolk</trd>
  <trdgrp lng="pl">
    <trd>chmura pierzasta</trd>,
    <trd>cirrus <klr>(Ci)</klr></trd>
  </trdgrp>
  <trd lng="pt">cirro</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_i;&c_s;&c_t;&c_o;&c_je;
  &c_o;&c_b;&c_l;&c_a;&c_k;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>cirus</trd>,
    <trd>riasovit&yacute; oblak</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x435;&#x440;&#x438;&#x441;&#x442;&#x430; (&#x430;&#x431;&#x43E; &#x43F;&#x456;&#x440;&#x2019;&#x44F;&#x441;&#x442;&#x430;) &#x445;&#x43C;&#x430;&#x440;&#x430;</trd>
</drv>
</art>
