<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>greft</rad>/i
</kap>
<drv mrk="greft.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="greft.0i.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Enigi vivantan bran&ccirc;eton a&ubreve; bur&gcirc;onon en alian 
      vegeta&jcirc;on, tiel ke &gcirc;i plue kresku:
      <ekz><tld/>i arbon, sova&gcirc;an pomujon, rozujon;</ekz>
      <ekz><tld/>i bran&ccirc;on sur alispecan trunkon.</ekz>
    </dif>
    <ref tip="sin" cel="inokul.0i.grefti">inokuli 1.</ref>
    <trdgrp lng="de">
     <trd>pfropfen</trd>,
     <trd>veredeln</trd>,
     <trd>pelzen</trd></trdgrp>
    <trd lng="ru">&c_p;&c_r;&c_i;&c_v;&c_i;&c_t;&c_mol; <klr>(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  </snc>
  <snc mrk="greft.0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Transporti vivantan parton de animala korpo sur alian:
      <ekz><tld/>i glandon, ha&ubreve;tpecon.</ekz>
    </dif>
    <trdgrp lng="de">
     <trd>transplantieren</trd>,
     <trd>verpflanzen</trd>,
     <trd>&uuml;bertragen</trd></trdgrp>
    <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_s;&c_a;&c_d;&c_i;&c_t;&c_mol; <klr>(&c_o;&c_r;&c_g;&c_a;&c_n;)</klr></trd>
  </snc>
  <trdgrp lng="fr">
    <trd>greffer</trd>,
    <trd>enter</trd>,
    <trd>transplanter<klr tip="ind"> (greffer)</klr></trd></trdgrp> 
</drv>
<drv mrk="greft.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">BOT</uzo>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      En<tld/>ita bran&ccirc;eto, bur&gcirc;on a&ubreve; histo.  
    </dif>
  </snc>
    <trdgrp lng="de">
     <trd>Pfropfreis</trd>,
     <trd>Edelreis</trd>,
     <trd>Transplantat</trd></trdgrp>
  <trdgrp lng="fr">
    <trd>greffon</trd>,
    <trd>transplant</trd></trdgrp> 
  <trd lng="ru">&c_p;&c_r;&c_i;&c_v;&c_o;&c_j;</trd>
</drv>
</art>
