<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nanken.xml,v 1.20 2022/05/01 16:20:54 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>nanken</rad>/o
</kap>
<drv mrk="nanken.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">TEKS</uzo>
   <snc>
     <dif>
       Unukolora, ordinare helflava teksa&jcirc;o el
       <ref tip="vid" cel="koton.0o">kotono</ref>,
       origine fabrikata en la &ccirc;ina urbo <ref cel="nankin.0o">Nankino</ref>:
      <ekz>
        precipe en la 19-a jarcento en E&ubreve;ropo malpezaj someraj vesta&jcirc;oj el <tld/>o estis
        aparte &scirc;atataj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=441313">Nankeno</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        indieno, kalikoto, kretono, <tld/>o kaj perkalo estas <ref tip="super" cel="katun.0o">katuno</ref>j
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Katuno">Katuno</url></vrk>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trdgrp lng="ca">
     <trd>alanquins</trd>,
     <trd>nanquins</trd>
   </trdgrp>
  <trd lng="cs">nankin</trd>
  <trd lng="de">Nanking</trd>
  <trd lng="en">nankeen</trd>
  <trd lng="fr">nankin</trd>
  <trd lng="hu">nanken</trd>
  <trd lng="nl">nanking</trd>
  <trd lng="pl">nankin</trd>
  <trd lng="pt">nanquim <klr>(tecido)</klr></trd>
  <trd lng="ru">&c_n;&c_a;&c_n;&c_k;&c_a;</trd>
  <trd lng="sk">nankin</trd>
  <trd lng="uk">&#x43D;&#x430;&#x43D;&#x43A;&#x430; (&#x442;&#x43A;&#x430;&#x43D;&#x438;&#x43D;&#x430;)</trd>
</drv>
</art>
