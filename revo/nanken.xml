<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>nanken</rad>/o
</kap>
<drv mrk="nanken.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">TEKS</uzo>
   <snc>
     <dif>
       Unukolora, ordinare helflava teksa&jcirc;o el
       <ref tip="vid" cel="koton.0o">kotono</ref>,
       origine fabrikata en la &ccirc;ina urbo Nankeno.
     </dif>
   </snc>
   <trd lng="de">Nanking</trd>
   <trd lng="en">nankeen</trd>
   <trd lng="fr">nankin</trd>
   <trd lng="hu">nanken</trd>  <trd lng="nl">nanking</trd>
   <trd lng="pt">nanquim <klr>(tecido)</klr></trd>
   <trd lng="ru">&c_n;&c_a;&c_n;&c_k;&c_a;</trd>
</drv>
</art>
