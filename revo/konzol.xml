<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>konzol</rad>/o
</kap>
<drv mrk="konzol.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Elstaranta &scirc;tona, metala a&ubreve; ligna peco, kurbigita la&ubreve; S-formo,
      fiksita al muro, kaj destinita por subporti balkonon, kornicon,
      statuon ktp.
    </dif>
  </snc>
  <snc>
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Ornama meblo, metita &ccirc;e muro, kaj destinita por subporti vazojn,
      kalikojn, statuetojn ktp.
    </dif>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_n;&c_s;&c_o;&c_l;&c_mol;</trd>,
    <trd>&c_k;&c_r;&c_o;&c_n;&c_sh;&c_t;&c_je;&c_j;&c_n;</trd>
  </trdgrp>
</drv>
</art>
