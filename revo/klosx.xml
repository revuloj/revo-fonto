<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>klo&scirc;</rad>/o
</kap>
<drv mrk="klosx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="klosx.0o.ujo">
    <dif>
      Ujo, &gcirc;enerale vitra kaj sonorilforma, por kovri kaj &scirc;irmi
      froma&gcirc;on,
      horlo&gcirc;on, delikatan vegeta&jcirc;on ktp.
    </dif>
    <trdgrp lng="en">
      <trd>cloche</trd>,
      <trd>bell jar</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>cloche<klr tip="ind"> (sorte de couvercle)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="klosx.0o.formo">
    <dif>
      Samforma objekto:
      <ekz>
        la jasmenaj floroj malfermis siajn blankajn <tld/>ojn<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>clochette<klr tip="ind"> (fleur)</klr></trd>,
      <trd>objet en forme de <ind>cloche</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="klosx.0o.MUZ">
    <fnt><bib>MuzTerm</bib></fnt>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Pli lar&gcirc;a, sonoron ellasanta ekstremo de pluraj
      <ref tip="malprt" cel="instru2.blov0o">blovinstrumentoj</ref>.
    </dif>
    <trdgrp lng="fr">
      <trd>pavillon<klr tip="ind"> (d'instrument de musique)</klr></trd>
    </trdgrp>
    <trd lng="en">bell</trd>
  </snc>
  <trd lng="de">Glocke</trd>
  <trd lng="nl">stolp</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_l;&c_p;&c_a;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_r;&c_y;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_o;&c_k;&c_o;&c_l;</trd>
  </trdgrp>
</drv>
</art>
