<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>klo&scirc;</rad>/o
</kap>
<drv mrk="klosx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Ujo, &gcirc;enerale vitra kaj sonorilforma, por kovri kaj &scirc;irmi froma&gcirc;on,
      horlo&gcirc;on, delikatan vegeta&jcirc;on ktp. 
    </dif>
    <trdgrp lng="en">
      <trd>cloche</trd>,
      <trd>bell jar</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <dif>
      Samforma objekto:
      <ekz>
        la jasmenaj floroj malfermis siajn blankajn <tld/>ojn<fnt>Z</fnt>.
      </ekz>  
    </dif>
  </snc>
  <trd lng="de">Glocke</trd>
  <trd lng="nl">stolp</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_l;&c_p;&c_a;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_r;&c_y;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_o;&c_k;&c_o;&c_l;</trd>
  </trdgrp>
</drv>
</art>
