<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ciprin1.xml,v 1.13 2022/03/14 15:25:39 revo Exp $">
<kap>
  <rad>ciprined</rad>/oj
</kap>
<drv mrk="ciprin1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo de <ref tip="malprt" cel="ciprin.0oformaj">ciprinoformaj
      fi&scirc;oj</ref> (<trd lng="la">Cyprinidae</trd>):
      <ekz>
        multaj fi&scirc;aj specioj vivas en Gango, <klr>[&#x2026;]</klr>
        inter ili pluraj genroj de <tld lit="C"/>oj (<nom>Labeo</nom>, <nom>Barilius</nom>, <nom>Bangana</nom> ktp)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=31515">Gango</url></lok>
        </fnt>.
      </ekz>
    </dif> 
    <refgrp tip="prt">
      <ref cel="alburn.0o">alburno</ref>,
      <ref cel="ciprin.0o">ciprino</ref>
      (<ref cel="karp.0o">karpo</ref>),
      <ref cel="gobi.0o">gobio</ref>,
      <ref cel="karas.0o">karaso</ref>,
      <ref cel="leuxcisk.0o">le&ubreve;cisko</ref>,
      <ref cel="plot.0o">ploto</ref>,
      <ref cel="tink.0o">tinko</ref>,
      <ref cel="vimb.0o">vimbo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_p;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">cipr&iacute;nids</trd>
  <trd lng="de">Karpfenfische</trd>
  <trd lng="el">&kappa;&ypsilon;&pi;&rho;&jota;&ny;&omikron;&epsilon;&jota;&delta;&eta_ton;</trd>
  <trd lng="fr">cyprinid&eacute;s</trd>
  <trd lng="hu">pontyf&eacute;l&eacute;k</trd>
  <trd lng="it">ciprinidi</trd>
  <trd lng="pl">karpiowate</trd>
  <trd lng="pt">ciprin&iacute;deos</trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_p;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&#x43A;&#x43E;&#x440;&#x43E;&#x43F;&#x43E;&#x432;&#x456;</trd>
</drv>
</art>

