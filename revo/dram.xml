<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dram.xml,v 1.24 2023/08/31 10:44:02 revo Exp $">
<kap>
  <ofc>1</ofc><rad>dram</rad>/o
</kap>

<drv mrk="dram.0o">
  <kap><tld/>o</kap>
  <snc mrk="dram.0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="super" cel="teatr.0ajxo">Teatra&jcirc;o</ref>, precipe
      tia, en
      kia alternas sentoj, ne nur amuza (komedia) a&ubreve; serioza
      (tragedia):
      <ekz>
        la <tld/>oj de &Scirc;ekspiro.
      </ekz>
    </dif>
    <trd lng="ca">drama</trd>
    <trd lng="pl">dramat</trd>
  </snc>
  <snc mrk="dram.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Terura okazo.
    </dif>
    <refgrp tip="vid">
      <ref cel="fatal.0o">fatalo</ref>,
      <ref cel="katast.0o">katastrofo</ref>,
      <ref cel="traged.0o.FIG">tragedio <sncref/></ref>
    </refgrp>
    <trd lng="ca">desastre</trd>
    <trd lng="pl">dramat</trd>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_a;&c_m;&c_a;</trd>
  <trd lng="bg">&c_d;&c_r;&c_a;&c_m;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>drama</trd>,
    <trd>nap&#xED;nav&#xE1; situace</trd>,
    <trd>trag&#xE9;die</trd>
  </trdgrp>
  <trd lng="de">Drama</trd>
  <trd lng="en">drama</trd>
  <trd lng="es">drama</trd>
  <trd lng="fr">drame</trd>
  <trdgrp lng="ja">
    <trd>&#x6B63;&#x5287;  [<ind>&#x305B;&#x3044;&#x3052;&#x304D;</ind>]</trd>,
    <trd/>,
    <trd>&#x6F14;&#x5287;  [<ind>&#x3048;&#x3093;&#x3052;&#x304D;</ind>]</trd>,
    <trd>&#x622F;&#x66F2;  [<ind>&#x304E;&#x304D;&#x3087;&#x304F;</ind>]</trd>,
    <trd>&#x5287;&#x7684;&#x4E8B;&#x4EF6;  [<ind>&#x3052;&#x304D;&#x3066;&#x304D;&#x3058;&#x3051;&#x3093;</ind>]</trd>,
    <trd>&#x30C9;&#x30E9;&#x30DE;  [<ind>&#x3069;&#x3089;&#x307E;</ind>]</trd>
  </trdgrp>
  <trd lng="pt">drama</trd>
  <trd lng="ru">&c_d;&c_r;&c_a;&c_m;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>dr&#xE1;ma</trd>,
    <trd>trag&#xE9;dia</trd>
  </trdgrp>
  <trd lng="sv">drama</trd>
  <trd lng="uk">&#x434;&#x440;&#x430;&#x43C;&#x430;</trd>
</drv>

<drv mrk="dram.0a">
  <kap><tld/>a</kap>
  <snc mrk="dram.0a.teatra">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Teatra, rilata al <tld/>o.
    </dif>
    <trd lng="de">Theater-</trd>
    <trd lng="pl">dramatyczny</trd>
    <trd lng="sv">teatrisk</trd>
  </snc>
  <snc mrk="dram.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tre emociiga:
      <ekz>
        vi imagis ion <tld/>an, dum temas pri <klr>...</klr> tute natura
        okaza&jcirc;o
        <fnt><bib>Gerda</bib>, <lok>&ccirc;apitro 5a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">dramatisch</trd>
    <trd lng="pl">dramatyczny</trd>
    <trd lng="ru">&c_d;&c_r;&c_a;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
    <trd lng="sv">dramatisk</trd>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_a;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">dram&agrave;tic</trd>
  <trdgrp lng="cs">
    <trd>dramatick&#xFD;</trd>,
    <trd>tragick&#xFD;</trd>
  </trdgrp>
  <trd lng="en">dramatic</trd>
  <trd lng="es">dram&aacute;tico</trd>
  <trd lng="fr">dramatique</trd>
  <trd lng="he">&#x5D3;&#x5E8;&#x5DE;&#x5D8;&#x5D9;</trd>
  <trdgrp lng="ja">
    <trd>&#x6F14;&#x5287;&#x306E;  [<ind>&#x3048;&#x3093;&#x3052;&#x304D;&#x306E;</ind>]</trd>,
    <trd>&#x5287;&#x7684;&#x306A;  [<ind>&#x3052;&#x304D;&#x3066;&#x304D;&#x306A;</ind>]</trd>
  </trdgrp>
  <trd lng="sk">dramatick&#xFD;</trd>
  <trd lng="uk">&#x434;&#x440;&#x430;&#x43C;&#x430;&#x442;&#x438;&#x447;&#x43D;&#x438;&#x439;</trd>
</drv>

<drv mrk="dram.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi <tld/>a:
      <ekz>
        la romantikaj poetoj ofte <tld/>igis sian vivon.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_a;&c_m;&c_a;&c_t;&c_y;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">dramatitzar</trd>
  <trd lng="cs">zdramatizovat</trd>
  <trd lng="de">dramatisieren</trd>
  <trd lng="en">dramatize</trd>
  <trd lng="fr">dramatiser</trd>
  <trd lng="he">&#x5DC;&#x5B0;&#x5D4;&#x5B7;&#x5DE;&#x5D7;&#x5B4;&#x5D9;&#x5D6;</trd>
  <trdgrp lng="ja">
    <trd>&#x5287;&#x5316;&#x3059;&#x308B;  [<ind>&#x3052;&#x304D;&#x304B;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x811A;&#x8272;&#x3059;&#x308B;  [<ind>&#x304D;&#x3083;&#x304F;&#x3057;&#x3087;&#x304F;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="pl">dramatyzowa&cacute;</trd>
  <trd lng="ru">&c_d;&c_r;&c_a;&c_m;&c_a;&c_t;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">zdramatizova&#x165;</trd>
  <trd lng="sv">dramatisera</trd>
  <trd lng="uk">&#x434;&#x440;&#x430;&#x43C;&#x430;&#x442;&#x438;&#x437;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
</drv>
</art>
