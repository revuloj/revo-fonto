<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: elater.xml,v 1.12 2024/09/19 09:30:42 revo Exp $">
<kap>
  <rad>elater</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
  <drv mrk="elater.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Iu el la torditaj kaj elastaj <ref tip="super" cel="filame.0o.ANA">filamento</ref>j,
      kiuj fiksas &ccirc;e la placento la sporojn de la <ref tip="malprt"
      cel="musk.hepato0oj">hepatomuskoj</ref>
      kaj ilin for&jcirc;etas rekti&gcirc;ante:
      <ekz>
        en la ekvizetoj, <tld/>oj estas kvar ruband-similaj alpenda&jcirc;oj fiksitaj al la
        sporoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=427523">Elatero</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="elatr.0o">elatro, elatero (skarabo)</ref>
  <trdgrp lng="be">
    <trd>&c_e;&c_l;&c_ja;&c_t;&c_e;&c_r;&c_a;</trd>,
    <trd>&c_s;&c_p;&c_r;&c_u;&c_zh;&c_y;&c_n;&c_k;&c_a; <klr tip="amb">(&c_k;&c_l;&c_je;&c_t;&c_k;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="de">Elatere</trd>
  <trd lng="fr">&eacute;lat&egrave;re</trd>
  <trdgrp lng="ja">
    <trd>&#x30B3;&#x30E1;&#x30C4;&#x30AD;&#x30E0;&#x30B7;</trd>,
    <trd>&#x5F3E;&#x7CF8;  <pr>&#x305F;&#x307E;&#12356;&#12392;</pr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>elat&eacute;rio</trd>,
    <trd>el&aacute;tero</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&c_v;&c_a;&#x43B;&c_i;&#x43A;</trd>,
    <trd>&c_je;&#x43B;&c_a;&c_t;&c_je;&c_r;&c_a;</trd>
  </trdgrp>
</drv>
</art>
