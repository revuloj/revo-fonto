<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>bol&scirc;evik</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="bolsxevik.0o">
  <kap><tld/>o</kap>
  <snc mrk="bolsxevik.0o.POL">
    <ref tip="dif" cel="bolsxe1.0o">bol&scirc;evisto</ref>
    <ekz>
      Vi devas lerni de la <tld/>oj
      <fnt><bib>Metrop</bib></fnt>!
    </ekz>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_l;&c_mol;&c_sh;&c_a;&c_v;&c_ib;&c_k;</trd>
  <trdgrp lng="de">
    <trd>Bolschewik</trd>,
    <trd>Bolschewist</trd>
  </trdgrp>
  <trd lng="en">Bolshevik</trd>
  <trd lng="es">bolchevique</trd>
  <trdgrp lng="fr">
    <trd>bolchevik<klr> (subst.)</klr></trd>,
    <trd>bolchevique<klr> (subst.)</klr></trd>,
    <trd>bolcheviste<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="hu">bolsevik</trd>
  <trd lng="ru">&c_b;&c_o;&c_l;&c_mol;&c_sh;&c_je;&c_v;&c_i;&c_k;</trd>
  <trd lng="sk">bol&scaron;evik</trd>
</drv>

<drv mrk="bolsxevik.0a">
  <kap><tld/>a</kap>
  <snc mrk="bolsxevik.0a.POL">
    <dif>
      Bol&scirc;evista:
      <ekz>
        ili difektos tiun &ccirc;i admirindan pargeton kaj el la tuta palaco
        faros iun <tld/>an klubon
        <fnt><bib>Metrop</bib></fnt>!
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>bolchevik<klr> (adj.)</klr></trd>,
    <trd>bolchevique<klr> (adj.)</klr></trd>,
    <trd>bolcheviste<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
-->
</vortaro>