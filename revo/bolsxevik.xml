<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bolsxevik.xml,v 1.10 2024/12/04 20:30:05 revo Exp $">
<kap>
  <rad>bol&scirc;evik</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>

<drv mrk="bolsxevik.0o">
  <kap><tld/>o</kap>
  <snc mrk="bolsxevik.0o.POL">
    <dif>
      <ref tip="dif" cel="bolsxe1.0o">Bol&scirc;evisto</ref>:
      <ekz>
        vi devas lerni de la <tld/>oj
        <fnt><bib>Metrop</bib></fnt>!
      </ekz>
      <ekz>
        paradokse <klr>[&#8230;]</klr> la <tld/>oj neniam plu havis plimulton &ndash; fakte ili
        estis malplimultuloj, <ind>men&scirc;eviko</ind>j
        <fnt>
          <aut>Old&rcaron;ich Arno&scaron;t Fischer</aut>
          <vrk><url ref="http://esperantobrno.cz/malnova/fischer-dormo-racio-2022-01-25.pdf"
              >Dormo de racio en soveta biologio</url></vrk>
        </fnt>.
        <trd lng="de">Menschewik</trd>
        <trd lng="ru">&c_m;&c_je;&c_n;&c_mol;&c_sh;&c_je;&c_v;&c_i;&c_k;</trd>
        <!-- men&scirc;eviko &scirc;ajne trovi&gcirc;as en pliaj fontoj, i.a.
        https://files.libcom.org/files/libera-laboristo-1925-augusto-no-1.pdf
        https://files.libcom.org/files/libera-laboristo-1925-decembro-no-3.pdf
        do eble meritas sian propran artikolon
        -->
      </ekz>
    </dif>
    <trdgrp lng="zh">
      <trd>&#26497;&#31471;&#20221;&#23376; <pr>j&iacute;du&amacron;nf&egrave;nz&#464;</pr></trd>,
      <trd>&#33487;&#32852;&#20849;&#20135;&#20826;&#21592;
      <pr>s&umacron;li&aacute;ng&ograve;ngch&#462;nd&#462;ngyu&aacute;n</pr></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_l;&c_mol;&c_sh;&c_a;&c_v;&c_ib;&c_k;</trd>
  <trdgrp lng="de">
    <trd>Bolschewik</trd>,
    <trd>Bolschewist</trd>
  </trdgrp>
  <trd lng="en">Bolshevik</trd>
  <trd lng="es">bolchevique</trd>
  <trdgrp lng="fr">
    <trd>bolchevik<klr> (subst.)</klr></trd>,
    <trd>bolchevique<klr> (subst.)</klr></trd>,
    <trd>bolcheviste<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="he">&bet;&vav;&lamed;&shin;&bet;&yod;&zayin;&fmem;</trd>
  <trd lng="hu">bolsevik</trd>
  <trd lng="pl">bolszewik</trd>
  <trd lng="ru">&c_b;&c_o;&c_l;&c_mol;&c_sh;&c_je;&c_v;&c_i;&c_k;</trd>
  <trd lng="sk">bol&scaron;evik</trd>
  <trd lng="sv">bolsjevik</trd>
</drv>

<drv mrk="bolsxevik.0a">
  <kap><tld/>a</kap>
  <snc mrk="bolsxevik.0a.POL">
    <dif>
      Bol&scirc;evista:
      <ekz>
        la <tld/>a sturmo de la Vintra Palaco en 1917
        <fnt>
          <vrk><url
          ref="https://www.paivanetto.com/eo/karitato/jakobenoj-gilotino-kaj-la-forgesita-frateco">Jakobenoj,
          gilotino kaj la forgesita Frateco | Paiva Netto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ili difektos tiun &ccirc;i admirindan pargeton kaj el la tuta
        palaco faros iun <tld/>an klubon
        <fnt><bib>Metrop</bib></fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_l;&c_mol;&c_sh;&c_a;&c_v;&c_ib;&c_c;&c_k;&c_ib;</trd>
  <trdgrp lng="de">
    <trd>bolschewikisch</trd>,
    <trd>bolschewistisch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bolshevik<klr> (adj.)</klr></trd>,
    <trd>bolshevist<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bolchevik<klr> (adj.)</klr></trd>,
    <trd>bolchevique<klr> (adj.)</klr></trd>,
    <trd>bolcheviste<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="he">&bet;&vav;&lamed;&shin;&bet;&yod;&qof;</trd>
  <trd lng="pl">bolszewicki</trd>
  <trd lng="ru">&c_b;&c_o;&c_l;&c_mol;&c_sh;&c_je;&c_v;&c_i;&c_s;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sv">bolsjevikisk</trd>
</drv>
</art>

