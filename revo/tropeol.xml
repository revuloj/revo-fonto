<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tropeol.xml,v 1.6 2024/01/27 01:21:52 revo Exp $">
<kap>
  <rad>tropeol</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="tropeol.0o">
  <kap><tld/>o</kap>
  <snc mrk="tropeol.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Rampa a&ubreve; liana kreska&jcirc;o
      el Sudameriko, ofte kultivata pro belaj,
      flav- &gcirc;is oran&gcirc;koloraj floroj;
      genro el familio tropeolacoj
      (<trd lng="la">Tropaeolum</trd>):
      <ekz>
        vi havu dometon kun &gcirc;ardeno por planti viajn klematidojn, viajn
        <tld/>ojn
        <fnt>
          <aut>A. Dumas, trad. D. Moirand</aut>,
          <vrk>La grafo de Monte-Kristo</vrk>,
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_s;&c_t;&c_u;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_p;&c_u;&c_c;&c_y;&c_n; <klr tip="amb">(&c_k;&c_v;&c_je;&c_t;&c_k;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="fr">capucine</trd>
  <trdgrp lng="ja">
    <trd>&#x30AD;&#x30F3;&#x30EC;&#x30F3;&#x30AB;</trd>,
    <trd>&#x30CE;&#x30A6;&#x30BC;&#x30F3;&#x30CF;&#x30EC;&#x30F3;</trd>,
    <trd>&#x30CA;&#x30B9;&#x30BF;&#x30C1;&#x30A6;&#x30E0;</trd>
  </trdgrp>
  <trd lng="pl">nasturcja</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_r;&c_a;&c_s;&#x43E;&#x43B;&#x44F;</trd>,
    <trd>&#x43D;&c_a;&c_s;&c_t;&c_u;&c_r;&c_c;&c_ib;&#x44F;</trd>
  </trdgrp>
</drv>
</art>

