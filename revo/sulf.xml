<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>sulf</rad>/o
</kap>
<drv mrk="sulf.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Mallongigita nomo de la sulfuro, kiun oni uzas en la nomoj de &hcirc;emiaj
      kombinoj:
      <ekz>
        <tld/>ato;
      </ekz>
      <ekz>
        <tld/>ata, <tld/>ita anhidrido.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Schwefel</trd>
  <trd lng="nl">zwavel</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_r;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_l;&c_mol;&c_f;-</trd>
  </trdgrp>
</drv>
</art>
