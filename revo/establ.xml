<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>4</ofc>
<rad>establ</rad>/i
<fnt>Z</fnt>
</kap>
<drv mrk="establ.0i">
<kap><tld/>i</kap>
<gra><vspec>tr</vspec></gra>
<snc>
<dif>
Fondi k aran&gcirc;i organizon por ke &gcirc;i povu funkcii k da&ubreve;ri. 
</dif>
<refgrp tip="vid">
<ref cel="star.0igi">starigi</ref>,
<ref cel="instal.0i">instali</ref>,
<ref cel="ferm.mal0i">malfermi</ref>,
<ref cel="fond.0i">fondi</ref>.
</refgrp>
</snc>
  <trd lng="en">establish</trd>
  <trd lng="es">establecer</trd>
  <trd lng="fr">&eacute;tablir</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_s;&c_n;&c_o;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_ch;&c_r;&c_je;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_s;&c_t;&c_r;&c_a;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_l;&c_a;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_s;&c_t;&c_a;&c_n;&c_a;&c_v;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_k;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="establ.0ajxo">
<kap><tld/>(a&jcirc;)o<fnt>Z</fnt></kap>
<uzo tip="fak">EKON</uzo>
<uzo tip="fak">POL</uzo>
<snc>
<dif>
Da&ubreve;re funkcianta organizo.
</dif> 
<refgrp tip="vid">
<ref cel="instit.0o">institucio</ref>, 
<ref cel="instit1.0o">instituto</ref>, 
<ref cel="entrep.0o">entrepreno</ref>, 
<ref cel="firm1.0o">firmo</ref>.
</refgrp>
</snc>
  <trd lng="es">establecimiento</trd>
  <trd lng="fr">&eacute;tablissement<klr tip="ind"> (entreprise, organisation)</klr></trd>
  <trd lng="ru">&c_u;&c_ch;&c_r;&c_je;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>
<drv mrk="establ.0ejo">
<kap><tld/>ejo</kap>
<uzo tip="fak">EKON</uzo>
<uzo tip="fak">POL</uzo>
<snc>
<dif>
Restadejo de funkcianta organizo.  
</dif>
</snc>
  <trd lng="es">establecimiento</trd>
  <trd lng="fr">&eacute;tablissement<klr tip="ind"> (d'une entreprise, d'une organisation)</klr></trd>
</drv>
</art>
