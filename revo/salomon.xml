<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: salomon.xml,v 1.17 2023/09/03 12:09:43 revo Exp $">
<kap>
  <rad>Salomon</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="salomon.0o">
  <kap><tld/>o</kap>
  <snc mrk="salomon.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> hebredevena.
    </dif>
  </snc>
  <snc mrk="salomon.0o.PERS">
    <uzo tip="fak">BIB</uzo>
    <uzo tip="fak">PERS</uzo>
    <dif>
      <ref tip="lst" cel="regx.0o" lst="voko:historiaj_personoj">Re&gcirc;o</ref>
      de Izraelo (972 a.K. &ndash; 932 a.K.),
      filo de <ref tip="vid" cel="david.0o.PERS">Davido</ref>, figuro de
      sa&gcirc;a kaj potenca reganto en la Biblio kaj en la Korano:
      <ekz>
        la domo, kiun la re&gcirc;o <tld/>o konstruis por la Eternulo, havis
        la longon de sesdek ulnoj
        <fnt><bib>MT</bib>, <lok>&Re1; 6:2</lok></fnt>;
      </ekz>
      <ekz>
        la re&gcirc;ino de &Scirc;eba a&ubreve;dis la famon pri <tld/>o
        <fnt><bib>MT</bib>, <lok>&Re1; 10:10</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_S;&c_a;&c_l;&c_a;&c_m;&c_o;&c_n;</trd>
  <trd lng="cs">&Scaron;alamoun</trd>
  <trd lng="de">Salomon</trd>
  <trd lng="en">Solomon</trd>
  <trd lng="fr">Salomon</trd>
  <trd lng="hu">Salamon</trd>
  <trdgrp lng="id">
    <trd>Salomo</trd>,
    <trd>Sulaiman</trd>
  </trdgrp>
  <trd lng="it">Salomone</trd>
  <trd lng="ja">&#x30BD;&#x30ED;&#x30E2;&#x30F3;  [<ind>&#x305D;&#x308D;&#x3082;&#x3093;</ind>]</trd>
  <trd lng="pl">Salomon</trd>
  <trdgrp lng="pt">
   <trd>Salom&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_S;&c_o;&c_l;&c_o;&c_m;&c_o;&c_n;</trd>
  <trd lng="sk">&Scaron;alam&uacute;n</trd>
  <trd lng="uk">&c_S;&#x43E;&#x43B;&#x43E;&#x43C;&#x43E;&#x43D;</trd>
</drv>
</art>

