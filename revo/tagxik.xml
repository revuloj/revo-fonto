<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap><rad>ta&gcirc;ik</rad>/o</kap>

    <drv mrk="tagxik.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Ano de la &ccirc;efa etno de <tld lit="T"/>ujo, parolanta
          lingvon
          parencan al la persa:
          <ekz>
            nia centro anka&ubreve; havas problemojn, ekzemple: mankas
            informlibroj kaj vortaroj por <tld/>oj
            <fnt>
              <aut>Saidnabi Noebov</aut>:
              <vrk>Esperanto-Centro en Ta&gcirc;ikistano</vrk>.
              <bib>LOdE</bib>, <lok>2003. N-ro 1(99)</lok>
            </fnt>;
          </ekz><ekz>
            Natalija Kasymova estas <ind><tld/>ino</ind> kiu kantas
            &ccirc;i tie 
            kanzonojn de diversaj stiloj kaj landoj
            <fnt>
              <lok><url
              ref="http://esperanto-panorama.net/muziko/kasymova.htm"
                  >Kantas Natalija Kasymova</url></lok>
            </fnt>
            <trd
            lng="ru">&c_t;&c_a;&c_d;&c_zh;&c_i;&c_ch;&c_k;&c_a;</trd>
          </ekz>
        </dif>
        <trd lng="en">Tajik</trd>
        <trd lng="pl">Tad&zdot;yk</trd>
        <trdgrp lng="pt">
          <trd>tadjique</trd>,
        </trdgrp>
        <trd lng="ru">&c_t;&c_a;&c_d;&c_zh;&c_i;&c_k;</trd>
      </snc>
    </drv>

    <drv mrk="tagxik.T0ujo">
      <kap><tld lit="T"/>ujo</kap>
      <snc mrk="tagxik.T0ujo.GEO">
        <uzo tip="fak">GEOG</uzo>
        <uzo tip="fak">POL</uzo>
        <mlg>TJ</mlg>
        <dif>
          &Scirc;tato en la centra Azio kun &ccirc;efurbo
          Du&scirc;anbeo.
          Oficiala nomo: <em>Respubliko de Ta&gcirc;ikujo</em>.
          &Gcirc;is 1991
          <ref tip="vid" cel="sovet.S0unio">Sovetunia</ref> respubliko.
          &Gcirc;i limas kun <ref tip="vid"
          cel="afgan.0ujo.GEOG">Afganujo</ref>,
          <ref tip="vid" cel="cxin.Cx0ujo">&Ccirc;inujo</ref>,
          <ref tip="vid" cel="kirgiz.0ujo.GEOG">Kirgizujo</ref>
          kaj <ref tip="vid" cel="uzbek.0ujo">Uzbekujo</ref>.
        </dif>
        <trdgrp lng="pl">
          <trd>Tad&zdot;ykistan</trd>,
          <trd>Republika <ind>Tad&zdot;ykistanu</ind></trd>
        </trdgrp>
        <trd
        lng="ru">&c_T;&c_a;&c_d;&c_zh;&c_i;&c_k;&c_i;&c_s;&c_t;&c_a;&c_n;</trd>
      </snc>
    </drv>

    <drv mrk="tagxik.T0io">
      <kap>
        <tld lit="T"/>io
        <fnt>
          <vrk>Oficiala Informo de AdE</vrk>,
          <lok>numero 11</lok>
        </fnt>
      </kap>
      <snc mrk="tagxik.T0io.GEOG">
        <uzo tip="fak">GEOG</uzo>
        <uzo tip="fak">POL</uzo>
        <mlg>TJ</mlg>
        <dif>
          <ref tip="dif" cel="tagxik.T0ujo.GEOG">Ta&gcirc;ikujo</ref>
        </dif>
        <trdgrp lng="pl">
          <trd>Tad&zdot;ykistan</trd>,
          <trd>Republika <ind>Tad&zdot;ykistanu</ind></trd>
        </trdgrp>
        <trd
        lng="ru">&c_T;&c_a;&c_d;&c_zh;&c_i;&c_k;&c_i;&c_s;&c_t;&c_a;&c_n;</trd>
      </snc>
    </drv>

  </art>

