<drv mrk="eben.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="eben.0igi.platigi">
    <dif>
      Igi <ref tip="vid" cel="eben.0a.plata"><tld/>a<sncref/></ref>:
      <ekz>
        <tld/>igi vojon;
      </ekz>
      <ekz>
        &ccirc;io tie estis <tld/>igita kun la tero
        <fnt><bib>Fab2</bib>, <lok>Malnova domo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="eben.0igi.FIG">
    <dif>
      Igi <ref tip="vid" cel="eben.0a.facila"><tld/>a<sncref/></ref>:
      <ekz>
        <tld/>igi &ccirc;iujn malfacila&jcirc;ojn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="glat.0igi">glatigi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_r;&c_a;&c_w;&c_n;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_r;&c_a;&c_w;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_g;&c_l;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>plan&#xFD;rovat</trd>,
    <trd>srovnat povrch</trd>,
    <trd>urovnat</trd>,
    <trd>vyrovnat</trd>
  </trdgrp>
  <trd lng="de">ebenen</trd>
  <trdgrp lng="en">
    <trd>level</trd>,
    <trd>smooth</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_h;&a_m;&a_w;&a_A;&a_r; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_m;&a_s;&a_T;&a_H; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">aplanir</trd>
  <trdgrp lng="he">
    <trd>&#x5DC;&#x5D9;&#x5D9;&#x5E9;&#x5E8;</trd>,
    <trd>&#x5DC;&#x5E4;&#x5DC;&#x5E1;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>elegyenget</trd>,
    <trd>plan&iacute;roz</trd>
  </trdgrp>
  <trd lng="nl">vlak maken</trd>
  <trdgrp lng="pl">
    <trd>r&oacute;wna&cacute;</trd>,
    <trd>wyr&oacute;wnywa&cacute;</trd>,
    <trd>sp&lstroke;aszcza&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_r;&c_o;&c_v;&c_n;&c_ja;&c_t;&c_mol;</trd>
  <trd lng="sk">vyrovn&#xE1;va&#x165;</trd>
  <trd lng="uk">&#x432;&#x438;&#x440;&#x456;&#x432;&#x43D;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>
</drv>

