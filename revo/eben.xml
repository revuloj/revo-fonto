<drv mrk="eben.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="eben.0a.plata">
    <dif>
      <klr>(pri supra&jcirc;o)</klr> Tia, ke &ccirc;iuj &gcirc;iaj punktoj
      estas proksimume &ccirc;e la sama nivelo sen kava&jcirc;oj
      a&ubreve; elstara&jcirc;oj:
      <ekz>
        li malsupreniris kun ili, kaj staris sur <tld/>a loko
        <fnt>
          <bib>NT</bib>
          <lok>Luko 6:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        dentoj tiel blankaj kaj <tld/>aj
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a kiel planko
        <fnt>
          <bib>HsT</bib>
          <lok>Feli&ccirc;a homo</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la <tld/>a tero sub ni ne trovi&gcirc;as multe da vidinda&jcirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;irka&ubreve;a&jcirc;o, kvankam <tld/>a kaj unuforme distran&ccirc;ita de la kanala reto, estis
        interesa
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la deklivo fini&gcirc;as, la vojo fari&gcirc;as <tld/>a
        <fnt>
          <bib>FSI</bib>
          <lok>V</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="glat.0a">glata</ref>,
      <ref cel="plat.0a">plata</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_p;&c_l;&c_o;&c_s;&c_k;&c_ib;</trd>,
      <trd>&c_p;&c_l;&c_ja;&c_s;&c_k;&c_a;&c_t;&c_y;</trd>,
      <trd>&c_r;&c_o;&c_w;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>eben</trd>,
      <trd>glatt</trd>,
      <trd>flach</trd>,
      <trd>platt</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>even</trd>,
      <trd>level</trd>,
      <trd>smooth</trd>,
      <trd>plane</trd>
    </trdgrp>
    <trd lng="fa">&a_h;&a_m;&a_w;&a_A;&a_r;</trd>
    <trdgrp lng="fr">
      <trd>&eacute;gal<klr tip="ind"> (plan)</klr></trd>,
      <trd>plan<klr tip="ind"> (adj.)</klr></trd>,
      <trd>plat<klr tip="ind"> (adj.)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>s&iacute;k</trd>,
      <trd>sima</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>r&oacute;wny</trd>,
      <trd>p&lstroke;aski</trd>
    </trdgrp>
  <trdgrp lng="pt">
    <trd>plano</trd>,
    <trd>chato</trd>,
    <trd>liso</trd>,
    <trd>igual <klr tip="amb">(superf&iacute;cie)</klr></trd>,
    <trd>ch&atilde;o <klr tip="amb">(adj.)</klr></trd>,
  </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_o;&c_s;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_r;&c_o;&c_v;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x8E27;&#x8E27; <pr>d&#xED;d&#xED;</pr></trd>,
    <trd>&#38918;&#38918;&#21033;&#21033; <pr>sh&#xF9;nsh&#xF9;nl&#xEC;l&#xEC;</pr></trd>,
    <trd>&#25153;&#x5E73;&#x578B; <pr>bi&#x1CE;np&#xED;ngx&#xED;ng</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="eben.0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 38</lok></fnt>
    <dif>
      Rilata al <ref cel="eben.0o"><tld/>o</ref>;
      inkluzivata de <tld/>o:
      <ekz>
        <ref cel="simetr.0o.orta"><tld/>a simetrio</ref>;
      </ekz>
      <ekz>
        <tld/>a a&ubreve; ne<tld/>a <ref cel="kurb.0o.MAT">kurbo</ref>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_l;&c_o;&c_s;&c_k;&c_ib;</trd>
    <trd lng="de">eben</trd>
    <trd lng="en">plane</trd>
    <trd lng="fa">&a_m;&a_s;&a_T;&a_H;</trd>
    <trd lng="fr">plan<klr tip="ind"> (adj., math.)</klr></trd>
    <trd lng="pl">p&lstroke;aszczyznowy</trd>
    <trd lng="pt">plano</trd>
    <trd lng="ru">&c_p;&c_l;&c_o;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
  <snc mrk="eben.0a.facila">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref cel="form.unu0a">Unuforma</ref>, facila; prezentanta nenian malglata&jcirc;on:
      <ekz>
        resti sur la <tld/>a <klr>(senriska)</klr> vojo de la amikeco
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Esperanto-Movado prezenti&gcirc;as ne kiel <tld/>a
        surfaco, sed kiel konstrua&jcirc;o de
        buntaj diversecoj kaj malsamecoj
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="glat.0a">glata</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_g;&c_l;&c_a;&c_d;&c_k;&c_ib;</trd>,
      <trd>&c_r;&c_o;&c_w;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="fa">&a_S;&a_A;&a_f;</trd>
    <trd lng="fr">lisse</trd>
    <trdgrp lng="pl">
      <trd>r&oacute;wny</trd>,
      <trd>p&lstroke;aski</trd>
    </trdgrp>
    <trd lng="pt">f&aacute;cil</trd>
    <trdgrp lng="ru">
      <trd>&c_g;&c_l;&c_a;&c_d;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_r;&c_o;&c_v;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
    <trd lng="zh">&#20809;&#x6ED1; <pr>gu&amacron;nghu&#xE1;</pr></trd>
</snc>
  <trdgrp lng="cs">
    <trd>ploch&yacute;</trd>,
    <trd>rovinn&yacute;</trd>,
    <trd>rovn&yacute;</trd>
  </trdgrp>
  <trd lng="de">glatt</trd>
  <trd lng="hu">s&iacute;k-</trd><trd lng="hu">sima</trd>
  <trdgrp lng="ja">
    <trd>&#x5E73;&#12425;&#x306A;  <pr>&#x305F;&#12356;&#12425;&#x306A;</pr></trd>,
    <trd>&#x5E73;&#22374;&#x306A;  <pr>&#12408;&#12356;&#x305F;&#12435;&#x306A;</pr></trd>,
    <trd>&#x306A;&#12417;&#12425;&#x304B;&#x306A;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vlak<klr tip="ind"> (nw.)</klr></trd>,
    <trd>effen</trd>,
    <trd>plat</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hladk&yacute;</trd>,
    <trd>ploch&yacute;</trd>,
    <trd>rovn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_ib;&c_v;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_l;&c_o;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_g;&c_l;&c_a;&c_d;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_r;&c_ib;&c_v;&c_n;&c_i;&c_n;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="eben.0o">
  <kap><tld/>o</kap>
  <snc mrk="eben.0o.tereno">
    <dif>
      <ref tip="dif" cel="eben.0ajxo">Ebena&jcirc;o</ref>:
      <ekz>
        ili trovi&gcirc;is sur la malgranda <tld/>o sur la orienta bordo de la rivero
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        post kelkaj kilometroj la valo tute forpu&scirc;as la altrudemajn montojn kaj fari&gcirc;as
        vastega <tld/>o <klr>[&#8230;]</klr> kie somere la greno ondadas
        <fnt>
          <bib>Tor</bib>
          <lok>I.</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili lasis la pli supran <tld/>on kaj iris la&ubreve; sinua montpado malsupren
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Kunvoja&gcirc;antoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> kulturis la <tld/>on
        <fnt>
          <bib>ViV</bib>
          <lok>II.</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="etagx.0o.nivelo">eta&gcirc;o<sncref/></ref>,
      <ref cel="teras.0o.ebeno">teraso<sncref/></ref>
    </refgrp>
    <trd lng="de">Flachland</trd>
    <trdgrp lng="zh">
    <trd>&#x5E73;&#x5DDD; <pr>p&#xED;ngchu&amacron;n</pr></trd>,
    <trd>&#x91CE; <pr>y&#x11B;</pr></trd>,
    <trd>&#x5E73;&#x91CE; <pr>p&#xED;ngy&#x11B;</pr></trd>,
    <trd>&#x575D; <pr>b&#xE0;</pr></trd>,
    <trd>&#x57EB; <pr>t&#x1CE;ng</pr></trd>,
    <trd>&#x5E73;&#x539F; <pr>p&#xED;ngyu&#xE1;n</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="eben.0o.elementa">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 25</lok></fnt>
    <dif>
      <ref tip="super" cel="surfac.0o.MAT">Surfaco</ref>, kiu inkluzivas
      &ccirc;iun <ref cel="rekt.0o.elementa">rekton</ref> trairantan du
      punktojn en &gcirc;i:
      <ekz>
        tra tri punktoj povas trairi nur unu <tld/>o.
      </ekz>
    </dif>
  </snc>
  <snc mrk="eben.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <ref cel="eben.afina0o">Afina <tld/>o</ref> a&ubreve;
      <ref cel="eben.vektora0o">vektora <tld/>o</ref>,
      la&ubreve; la kunteksto.
    </dif>
    <trd lng="zh">&#x5E73;&#38754; <pr>p&#xED;ngmi&#xE0;n</pr></trd>
</snc>
  <trd lng="be">&c_p;&c_l;&c_o;&c_s;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>plocha</trd>,
    <trd>plo&scaron;ina</trd>,
    <trd>rovina</trd>
  </trdgrp>
  <trd lng="de">Ebene</trd>
  <trd lng="en">plane<klr tip="ind"> (geom.)</klr></trd>
  <trd lng="es">plano</trd>
  <trd lng="fa">&a_S;&a_f;&a_H;&a_h;<klr tip="ind"> (&a_h;&a_n;&a_d;&a_s;&a_h;)</klr></trd>
  <trd lng="fr">plan<klr tip="ind"> (subst., g&eacute;om.)</klr></trd>
  <trd lng="hu">s&iacute;k</trd>
  <trd lng="ja">&#x5E73;&#38754;  <pr>&#12408;&#12356;&#12417;&#12435;</pr></trd>
  <trd lng="nl">plat <ind>vlak</ind></trd>
  <trd lng="pl">p&lstroke;aszczyzna</trd>
  <trd lng="ru">&c_p;&c_l;&c_o;&c_s;&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>planina</trd>,
    <trd>plocha</trd>,
    <trd>rovina</trd>
  </trdgrp>
</drv>

<drv mrk="eben.0ajxo">
  <kap><tld/>a&jcirc;o,
    <var><kap><tld/>ejo</kap></var>
  </kap>
  <snc mrk="eben.0ajxo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <tld lit="E"/>a <ref tip="super" cel="teren.0o.terpeco">tereno</ref>:
      <ekz>
        ni prenis tiam el la manoj de la du re&gcirc;oj <klr>[&#8230;]</klr> &ccirc;iujn urbojn sur la
        <tld/>a&jcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 3:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> mar&scirc;is sur la <tld/>a&jcirc;o tiun tutan nokton kaj transiris Jordanon
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 2:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> haltis &ccirc;e la supro de &scirc;tuparo, kondukanta de teraso al
        malgranda, artefarita <tld/>a&jcirc;o kun multekoloraj kaj simetrie aran&gcirc;itaj
        florbedoj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur tiu tereno <klr>[&#8230;]</klr> vi estos pli laca, ol dum vi kuris senturne kaj
        senbare sur la <tld/>ejo
        <fnt>
          <bib>BonaLingvo</bib>
          <lok>3. Natura Sin-esprimado</lok>
        </fnt>;
      </ekz>
      <ekz>
        la mal<tld/>a&jcirc;o fari&gcirc;u <tld/>a&jcirc;o, kaj la montaro fari&gcirc;u valo
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 40:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> maro vasti&gcirc;is &ccirc;irka&ubreve;e, kiel smeralda <tld/>a&jcirc;o
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        li havis <klr>[&#8230;]</klr> sub si la urbon kaj la vaste etendi&gcirc;antan <tld/>a&jcirc;on de
        Lombardio
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la okuloj medite rigardis la <tld/>a&jcirc;on, kiu etendis sin anta&ubreve; li
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LXXI</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
