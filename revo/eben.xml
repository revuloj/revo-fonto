<drv mrk="eben.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="eben.0igi.platigi">
    <dif>
      Igi <ref tip="vid" cel="eben.0a.plata"><tld/>a<sncref/></ref>:
      <ekz>
        <tld/>igi vojon;
      </ekz>
      <ekz>
        &ccirc;io tie estis <tld/>igita kun la tero
        <fnt><bib>Fab2</bib>, <lok>Malnova domo</lok></fnt>;
      </ekz>
      <ekz>
        sur la tuta areno eksvarmis febra agado <klr>[&#8230;oni]</klr> trarastis, <tld/>igis
        kaj sur&scirc;utis per dika tavolo da fre&scirc;a sablo
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi nun <tld/>igis kaj glatigis la tutspacon de la boato &gcirc;is la maro, por ke la
        boato gliti&gcirc;u maren
        <fnt>
          <bib>Rob</bib>
          <lok>Parto III</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="nivel.0i.horizontaligi">niveli<sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>einebnen</trd>,
      <trd>gl&auml;tten</trd>
    </trdgrp>
  </snc>
  <snc mrk="eben.0igi.FIG">
    <dif>
      Igi <ref tip="vid" cel="eben.0a.facila"><tld/>a<sncref/></ref>:
      <ekz>
        justeco de piulo <tld/>igas lian vojon
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 11:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        naivulo! &ndash; li diris, &ndash; kaj bona knabo, vi ja <tld/>igos al vi la vojon tra la
        mondo
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu heroa&jcirc;o <tld/>igis al francoj la vojon al &ccirc;efurbo <nom>Madrid</nom>
        <fnt>
          <bib>SrT</bib>
          <lok>Libro XI.</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="glat.0igi">glatigi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_r;&c_a;&c_w;&c_n;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_r;&c_a;&c_w;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_g;&c_l;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>plan&yacute;rovat</trd>,
    <trd>srovnat povrch</trd>,
    <trd>urovnat</trd>,
    <trd>vyrovnat</trd>
  </trdgrp>
  <trd lng="de">ebenen</trd>
  <trdgrp lng="en">
    <trd>level</trd>,
    <trd>smooth</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_h;&a_m;&a_w;&a_A;&a_r; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_m;&a_s;&a_T;&a_H; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">aplanir</trd>
  <trdgrp lng="he">
    <trd>&lamed;&yod;&yod;&shin;&resh;</trd>,
    <trd>&lamed;&pe;&lamed;&samekh;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>elegyenget</trd>,
    <trd>plan&iacute;roz</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5E73;&#12425;&#x306B;&#12377;&#x308B;  <pr>&#x305F;&#12356;&#12425;&#x306B;&#12377;&#x308B;</pr></trd>,
    <trd>&#22320;&#x306A;&#12425;&#12375;&#12377;&#x308B;  <pr>&#12376;&#x306A;&#12425;&#12375;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">vlak maken</trd>
  <trdgrp lng="pl">
    <trd>r&oacute;wna&cacute;</trd>,
    <trd>wyr&oacute;wnywa&cacute;</trd>,
    <trd>sp&lstroke;aszcza&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_r;&c_o;&c_v;&c_n;&c_ja;&c_t;&c_mol;</trd>
  <trd lng="sk">vyrovn&aacute;va&tcaron;</trd>
  <trd lng="uk">&c_v;&c_i;&c_r;&c_ib;&c_v;&c_n;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

