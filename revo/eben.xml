<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>eben</rad>/a
</kap>
<drv mrk="eben.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      (p.p. supra&jcirc;o) Tia, ke &ccirc;iuj &gcirc;iaj punktoj estas
      proksimume &ccirc;e la sama nivelo sen kava&jcirc;oj a&ubreve;
      elstara&jcirc;oj:
      <ekz>
        la supra&jcirc;o de la maro estas hodia&ubreve;
        <tld/>a;
      </ekz>
      <ekz>
        mal<tld/>a regiono.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="plat.0a">plata</ref>,
      <ref cel="glat.0a">glata</ref>.
    </refgrp>
    <trd lng="de">eben</trd>
    <trdgrp lng="fr">
      <trd>&eacute;gal<klr tip="ind"> (plan)</klr></trd>,
      <trd>plan<klr tip="ind"> (adj.)</klr></trd>,
      <trd>plat<klr tip="ind"> (adj.)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_o;&c_s;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_r;&c_o;&c_v;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <uzo tip="fak">MAT</uzo>
        <ref cel="simetr.0o.orta"><tld/>a simetrio</ref>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Facila, prezentanta nenian malhelpa&jcirc;on:
      <ekz>
        flua kaj <tld/>a stilo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="glat.0a">glata</ref>.
    </refgrp>
    <trd lng="fr">lisse</trd>
  </snc>
  <trd lng="de">glatt</trd>
  <trd lng="en">even</trd>
</drv>
<drv mrk="eben.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="eben.0o.elementa">
    <uzo tip="klr">(elementa geometrio)</uzo>
    <dif>
      <ref tip="super" cel="surfac.0o.MAT">Surfaco</ref>, kiu inkluzivas
      &ccirc;iun <ref cel="rekt.0o.elementa">rekton</ref> trairantan du
      punktojn en &gcirc;i:
      <ekz>
        tra tri punktoj povas trairi nur unu <tld/>o.
      </ekz>
    </dif>
  </snc>
  <snc mrk="eben.0o.MAT">
    <dif>
      <ref cel="eben.afina0o">Afina <tld/>o</ref> a&ubreve;
      <ref cel="eben.vektora0o">vektora <tld/>o</ref>,
      la&ubreve; la kunteksto.
    </dif>
  </snc>
  <trd lng="de">Ebene</trd>
  <trd lng="en">plane<klr tip="ind"> (geom.)</klr></trd>
  <trd lng="fr">plan<klr tip="ind"> (subst., g&eacute;om.)</klr></trd>
  <trd lng="ru">&c_p;&c_l;&c_o;&c_s;&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>
<drv mrk="eben.0ajxo">
  <kap><tld/>(a&jcirc;)o, <tld/>ejo</kap>
  <snc>
    <dif>
      Loko a&ubreve; lando <tld/>a:
      <ekz>
        la ruslanda <tld/>o estas
        plilongiga&jcirc;o de la vastega nordazia <tld/>o;
      </ekz>
      <ekz>
        Tibeto estas alt<tld/>a&jcirc;o.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Ebene</trd>
  <trd lng="fr">plateau<klr tip="ind"> (g&eacute;og.)</klr></trd>
  <trd lng="ru">&c_r;&c_a;&c_v;&c_n;&c_i;&c_n;&c_a;</trd>
</drv>
<drv mrk="eben.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi <tld/>a 1 kaj 2:
      <ekz>
        <tld/>igi vojon;
      </ekz>
      <ekz>
        &ccirc;io tie estis <tld/>igita kun la tero
        <fnt>(<aut>Zamenhof</aut>: <vrk>Fabeloj de Andersen,
        Malnova domo</vrk>)</fnt>;
      </ekz>
      <ekz>
        <tld/>igi &ccirc;iujn malfacila&jcirc;ojn
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="glat.0igi">glatigi</ref>.
    </refgrp>
  </snc>
  <trd lng="de">ebenen</trd>
  <trd lng="fr">aplanir</trd>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_r;&c_o;&c_v;&c_n;&c_ja;&c_t;&c_mol;</trd>
</drv>
<drv mrk="eben.0bati">
  <kap><tld/>bati<fnt>Z</fnt></kap>
  <snc>
    <dif>
      <tld lit="E"/>igi batante.
    </dif>
  </snc>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_l;&c_ju;&c_shch;&c_i;&c_t;&c_mol;</trd>
</drv>
<drv mrk="eben.sam0a">
  <kap>sam<tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      <klr>(p.p. <ref cel="punkt.0o.spaco">punktoj</ref> a&ubreve; aroj
      da punktoj)</klr>
      Apartenantaj al <klr>(a&ubreve; inkluzivataj en)</klr> unu sama 
      <ref cel="eben.0o.MAT"><tld/>o</ref>:
      <ekz>
        tri punktoj estas &ccirc;iam sam<tld/>aj;
      </ekz>
      <ekz>
        sam<tld/>aj kurboj;
      </ekz>
      <ekz>
        &ccirc;iu <ref cel="centr.pezo0o.MAT">pezocentro</ref> de tri punktoj 
        estas sam<tld/>a kun ili.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">komplanar</trd>
  <trd lng="en">coplanar</trd>
  <trd lng="fr">coplanaire</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_l;&c_a;&c_n;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>
</drv>
<drv mrk="eben.simetri0o">
  <kap>simetri<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="eben.0o.MAT"><tld lit="E"/>o</ref>, kiu difinas
      <ref cel="simetr.0o.orta"><tld/>an simetrion</ref>.
    </dif>
  </snc>
  <snc>
    <dif>
      <klr>(de <ref cel="figur.geometria0o">geometria figuro</ref>)</klr>
      <ref tip="super" cel="eben.0o.MAT"><tld lit="E"/>o</ref>, rilate al kiu
      la figuro estas <ref cel="simetr.0a.figuro">simetria</ref>.
    </dif>
  </snc>
  <trd lng="fr">plan de sym&eacute;trie</trd>
</drv>
<drv mrk="eben.afina0o">
  <kap>afina <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <ref cel="dimens.n0a">Dudimensia</ref>
      <ref cel="spac.afina0o">afina spaco</ref>.
    </dif>
  </snc>
  <trd lng="fr">plan affine</trd>
</drv>
<drv mrk="eben.vektora0o">
  <kap>vektora <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <ref cel="dimens.n0a">Dudimensia</ref>
      <ref cel="spac.vektora0o">vektora spaco</ref>.
    </dif>
  </snc>
  <trd lng="fr">plan vectoriel</trd>
</drv>
</art>
