<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>eben</rad>/a
</kap>
<drv mrk="eben.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc num="1">
    <dif>
      Epiteto karakterizanta supra&jcirc;on,  kies &ccirc;iuj punktoj estas
      proksimume &ccirc;e sama nivelo, prezentantan nek kava&jcirc;ojn nek
      elstara&jcirc;ojn:
      <ekz>
        la supra&jcirc;o de la maro estas hodia&ubreve;
        <tld/>a;
      </ekz>
      <ekz>
        mal<tld/>a regiono.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="plat.0a">plata</ref>,
      <ref cel="glat.0a">glata</ref>.
    </refgrp>
    <trd lng="de">eben</trd>
  </snc>
  <snc num="2">
    <uzo tip="klr">(f)</uzo>
    <dif>
      Facila, prezentanta nenian malhelpa&jcirc;on:
      <ekz>
        flua k <tld/>a stilo.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="glat.0a">glata</ref>.
    </refgrp>
  </snc>
  <trd lng="de">glatt</trd>
  <trd lng="en">even</trd>
</drv>
<drv mrk="eben.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      Supra&jcirc;o, kiun &ccirc;iupunkte tu&scirc;as &ccirc;iu rekta linio,
      kiu havas kun &gcirc;i du komunajn punktojn:
      <ekz>
        tra tri punktoj povas pasi nur unu <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Ebene</trd>
</drv>
<drv mrk="eben.0ajxo">
  <kap><tld/>(a&jcirc;)o, <tld/>ejo</kap>
  <snc>
    <dif>
      Loko a&ubreve; lando <tld/>a:
      <ekz>
        la ruslanda <tld/>o estas
        plilongiga&jcirc;o de la vastega nordazia <tld/>o;
      </ekz>
      <ekz>
        Tibeto estas alt<tld/>a&jcirc;o.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Ebene</trd>
</drv>
<drv mrk="eben.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi <tld/>a 1 k 2:
      <ekz>
        <tld/>igi vojon;
      </ekz>
      <ekz>
        &ccirc;io tie estis <tld/>igita kun la tero
        <fnt>(<aut>Zamenhof</aut>: <vrk>Fabeloj de Andersen,
        Malnova domo</vrk>)</fnt>;
      </ekz>
      <ekz>
        <tld/>igi &ccirc;iujn maljacila&jcirc;ojn
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="glat.0igi">glatigi</ref>.
    </refgrp>
  </snc>
  <trd lng="de">ebenen</trd>
</drv>
<drv mrk="eben.0bati">
  <kap><tld/>bati<fnt>Z</fnt></kap>
  <snc>
    <dif>
      <tld/>igi batante.  
    </dif>
  </snc>
</drv>
</art>
<!--
$Log$
-->
</vortaro>