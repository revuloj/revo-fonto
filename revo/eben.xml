  <trdgrp lng="uk">
    <trd>&#x43F;&#x43B;&#x43E;&#x441;&#x43A;&#x43E;&#x433;&#x456;&#x440;&#x2019;&#x44F;</trd>,
    <trd>&#x43F;&#x43B;&#x430;&#x442;&#x43E;</trd>
  </trdgrp>
</drv>

