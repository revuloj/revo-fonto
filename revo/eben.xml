<drv mrk="eben.alt0ajxo">
  <kap>alt<tld/>a&jcirc;o</kap>
  <snc mrk="eben.alt0ajxo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Ebena&jcirc;o tre alta super la marnivelo.
      <ekz>
        Tibeto estas alt<tld/>a&jcirc;o;
      </ekz>
      <ekz>
        li trarajdis la vastan alt<tld/>a&jcirc;on
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Pramaljuna Morlino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la siriaj Golan-alt<tld/>a&jcirc;oj estis konkeritaj de Israelo en 1967
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk>La blua &leftquot;oro&rightquot; &ccirc;iam malsufi&ccirc;os</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la toltekoj, <klr>[&#8230;]</klr> antikva indi&gcirc;ena popolo lo&gcirc;anta sur la
        alt<tld/>a&jcirc;o de Meksiko konis la
        pulkvon de anta&ubreve; pli ol 5000 jaroj
        <fnt>
          <bib>Monato</bib>
          <vrk>Centra Ameriko: Nepre &gcirc;ia nomo estas Meksiko</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ja;&c_t;&c_o;</trd>
  <trd lng="de">Hochebene</trd>
  <trd lng="fa">&a_f;&a_l;&a_A;&a_t;</trd>
  <trd lng="fr">plateau<klr tip="ind"> (g&eacute;og.)</klr></trd>
  <trd lng="he">&resh;&mem;&he;</trd>
  <trd lng="ja">&#x9AD8;&#x539F;  <pr>&#12371;&#12358;&#12370;&#12435;</pr></trd>
  <trd lng="nl">hoogvlakte</trd>
  <trd lng="pl">p&lstroke;askowy&zdot;</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_a;&c_t;&c_o;</trd>,
    <trd>&c_p;&c_l;&c_o;&c_s;&c_k;&c_o;&c_g;&c_o;&c_r;&c_mol;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_l;&c_o;&c_s;&c_k;&c_o;&c_g;&c_ib;&c_r;&#8217;&c_ja;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_t;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x576A; <pr>p&#xED;ng</pr></trd>,
    <trd>&#22596; <pr>l&#xE9;ng</pr></trd>,
    <trd>&#x53F0;&#22320; <pr>t&#xE1;id&#xEC;</pr></trd>,
    <trd>&#x9AD8;&#x539F; <pr>g&amacron;oyu&#xE1;n</pr></trd>,
    <trd>&#x586C; <pr>yu&#xE1;n</pr></trd>
  </trdgrp>
</drv>

