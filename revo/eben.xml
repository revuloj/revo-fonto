<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>eben</rad>/a
</kap>
<drv mrk="eben.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      <klr>(p.p. supra&jcirc;o)</klr> Tia, ke &ccirc;iuj &gcirc;iaj punktoj
      estas proksimume &ccirc;e la sama nivelo sen kava&jcirc;oj a&ubreve;
      elstara&jcirc;oj:
      <ekz>
        la supra&jcirc;o de la maro estas hodia&ubreve; <tld/>a;
      </ekz>
      <ekz>
        mal<tld/>a regiono.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="plat.0a">plata</ref>,
      <ref cel="glat.0a">glata</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>eben</trd>,
      <trd>glatt</trd>,
      <trd>flach</trd>,
      <trd>platt</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>even</trd>,
      <trd>level</trd>,
      <trd>smooth</trd>,
      <trd>plane</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>&eacute;gal<klr tip="ind"> (plan)</klr></trd>,
      <trd>plan<klr tip="ind"> (adj.)</klr></trd>,
      <trd>plat<klr tip="ind"> (adj.)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>s&iacute;k</trd>,
      <trd>sima</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_o;&c_s;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_r;&c_o;&c_v;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 38</lok></fnt>
    <dif>
      Rilata al <tld/>o; inkluzivata en <tld/>o:
      <ekz>
        <ref cel="simetr.0o.orta"><tld/>a simetrio</ref>;
      </ekz>
      <ekz>
        <tld/>a a&ubreve; ne<tld/>a <ref cel="kurb.0o.MAT">kurbo</ref>.
      </ekz>
    </dif>
    <trd lng="de">eben</trd>
    <trd lng="en">plane</trd>
    <trd lng="fr">plan<klr tip="ind"> (adj., math.)</klr></trd>
    <trd lng="ru">&c_p;&c_l;&c_o;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Facila, prezentanta nenian malhelpa&jcirc;on:
      <ekz>
        flua kaj <tld/>a stilo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="glat.0a">glata</ref>.
    </refgrp>
    <trd lng="fr">lisse</trd>
  </snc>
  <trd lng="de">glatt</trd>
  <trd lng="hu">s&iacute;k-</trd>
  <trdgrp lng="nl">
    <trd>vlak<klr tip="ind"> (nw.)</klr></trd>,
    <trd>effen</trd>,
    <trd>plat</trd>
  </trdgrp>
  <trd lng="hu">sima</trd>
</drv>

<drv mrk="eben.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="eben.0o.elementa">
    <fnt><bib>MatTerm</bib><lok>p. 25</lok></fnt>
    <dif>
      <ref tip="super" cel="surfac.0o.MAT">Surfaco</ref>, kiu inkluzivas
      &ccirc;iun <ref cel="rekt.0o.elementa">rekton</ref> trairantan du
      punktojn en &gcirc;i:
      <ekz>
        tra tri punktoj povas trairi nur unu <tld/>o.
      </ekz>
    </dif>
  </snc>
  <snc mrk="eben.0o.MAT">
    <dif>
      <ref cel="eben.afina0o">Afina <tld/>o</ref> a&ubreve;
      <ref cel="eben.vektora0o">vektora <tld/>o</ref>,
      la&ubreve; la kunteksto.
    </dif>
  </snc>
  <trd lng="de">Ebene</trd>
  <trd lng="en">plane<klr tip="ind"> (geom.)</klr></trd>
  <trd lng="fr">plan<klr tip="ind"> (subst., g&eacute;om.)</klr></trd>
  <trd lng="nl">plat <ind>vlak</ind></trd>
  <trd lng="hu">s&iacute;k</trd>
  <trd lng="ru">&c_p;&c_l;&c_o;&c_s;&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="eben.0ajxo">
  <kap><tld/>(a&jcirc;)o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Loko a&ubreve; lando <tld/>a:
      <ekz>
        la ruslanda <tld/>o estas
        plilongiga&jcirc;o de la vastega nordazia <tld/>o;
      </ekz>
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="eben.0ejo"><tld/>ejo</ref>
  </refgrp>
  <trd lng="de">Ebene</trd>
  <trd lng="en">plain</trd>
  <trd lng="fr">plaine</trd>
  <trd lng="nl">vlakte</trd>
  <trd lng="hu">s&iacute;ks&aacute;g</trd>
  <trd lng="ru">&c_r;&c_a;&c_v;&c_n;&c_i;&c_n;&c_a;</trd>
</drv>

<drv mrk="eben.0ejo">
  <kap><tld/>ejo</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="eben.0ajxo"><tld lit="E"/>a&jcirc;o</ref>
    </dif>
  </snc>
</drv>

<drv mrk="eben.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi <tld/>a 1 kaj 2:
      <ekz>
        <tld/>igi vojon;
      </ekz>
      <ekz>
        &ccirc;io tie estis <tld/>igita kun la tero
        <fnt><bib>Fab2</bib>, <lok>Malnova domo</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>igi &ccirc;iujn malfacila&jcirc;ojn
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="glat.0igi">glatigi</ref>.
    </refgrp>
  </snc>
  <trd lng="de">ebenen</trd>
  <trdgrp lng="en">
    <trd>level</trd>,
    <trd>smooth</trd>
  </trdgrp>
  <trd lng="fr">aplanir</trd>
  <trd lng="nl">vlak maken</trd>
  <trdgrp lng="hu">
    <trd>elegyenget</trd>,
    <trd>plan&iacute;roz</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_r;&c_o;&c_v;&c_n;&c_ja;&c_t;&c_mol;</trd>
</drv>

<drv mrk="eben.0bati">
  <kap><tld/>bati<fnt>Z</fnt></kap>
  <snc>
    <dif>
      <tld lit="E"/>igi batante.
    </dif>
  </snc>
  <trd lng="hu">sim&aacute;ra d&ouml;ng&ouml;l</trd>
  <trd
  lng="ru">&c_r;&c_a;&c_s;&c_p;&c_l;&c_ju;&c_shch;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="eben.alt0ajxo">
  <kap>alt<tld/>a&jcirc;o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Ebena&jcirc;o tre alta super la marnivelo.
      <ekz>
        Tibeto estas alt<tld/>a&jcirc;o.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">plateau<klr tip="ind"> (g&eacute;og.)</klr></trd>
  <trd lng="nl">hoogvlakte</trd>
</drv>

<drv mrk="eben.sam0a">
  <kap>sam<tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      <klr>(p.p. <ref cel="punkt.0o.spaco">punktoj</ref> a&ubreve; aroj
      da punktoj)</klr>
      Apartenantaj al <klr>(a&ubreve; inkluzivataj en)</klr> unu sama
      <ref cel="eben.0o.MAT"><tld/>o</ref>:
      <ekz>
        tri punktoj estas &ccirc;iam sam<tld/>aj;
      </ekz>
      <ekz>
        sam<tld/>aj kurboj;
      </ekz>
      <ekz>
        &ccirc;iu <ref cel="centr.pezo0o.MAT">pezocentro</ref> de tri
        punktoj
        estas sam<tld/>a kun ili.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">komplanar</trd>
  <trd lng="en">coplanar</trd>
  <trd lng="fr">coplanaire</trd>
  <trdgrp lng="hu">
    <trd>egy <ind>s&iacute;kba</ind> es&odblac;</trd>,
    <trd>k&ouml;z&ouml;s s&iacute;k&uacute;</trd>,
    <trd>komplan&aacute;ris</trd>
  </trdgrp>
  <trd
  lng="ru">&c_k;&c_o;&c_m;&c_p;&c_l;&c_a;&c_n;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="eben.simetri0o">
  <kap>simetri<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="eben.0o.MAT"><tld lit="E"/>o</ref>, kiu difinas
      <ref cel="simetr.0o.orta"><tld/>an simetrion</ref>.
    </dif>
  </snc>
  <snc>
    <dif>
      <klr>(de <ref cel="figur.geometria0o">geometria figuro</ref>)</klr>
      <ref tip="super" cel="eben.0o.MAT"><tld lit="E"/>o</ref>, rilate al
      kiu
      la figuro estas <ref cel="simetr.0a.figuro">simetria</ref>.
    </dif>
  </snc>
  <trd lng="de">Symmetrieebene</trd>
  <trd lng="en">plane of symmetry</trd>
  <trd lng="fr">plan de sym&eacute;trie</trd>
  <trd lng="hu">szimmetrias&iacute;k</trd>
  <trd lng="ru">&c_p;&c_l;&c_o;&c_s;&c_k;&c_o;&c_s;&c_t;&c_mol; &c_s;&c_i;&c_m;&c_m;&c_je;&c_t;&c_r;&c_i;&c_i;</trd>
</drv>

<drv mrk="eben.afina0o">
  <kap>afina <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      <ref cel="dimens.n0a">Dudimensia</ref>
      <ref cel="spac.afina0o">afina spaco</ref>.
    </dif>
  </snc>
  <trd lng="de">affine <ind>Ebene</ind></trd>
  <trd lng="en">affine <ind>plane</ind></trd>
  <trd lng="fr">plan affine</trd>
  <trd lng="hu">affin <ind>s&iacute;k</ind></trd>
  <trd lng="ru">&c_a;&c_f;&c_f;&c_i;&c_n;&c_n;&c_a;&c_ja; <ind>&c_p;&c_l;&c_o;&c_s;&c_k;&c_o;&c_s;&c_t;&c_mol;</ind></trd>
</drv>

<drv mrk="eben.vektora0o">
  <kap>vektora <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <ref cel="dimens.n0a">Dudimensia</ref>
      <ref cel="spac.vektora0o">vektora spaco</ref>.
    </dif>
  </snc>
  <trd lng="de">Vektorebene</trd>
  <trd lng="en">two-dimensional vector <ind>space</ind></trd>
  <trd lng="fr">plan vectoriel</trd>
  <trd lng="hu">vektors&iacute;k</trd>
  <trd lng="ru">&c_v;&c_je;&c_k;&c_t;&c_o;&c_r;&c_n;&c_a;&c_ja; <ind>&c_p;&c_l;&c_o;&c_s;&c_k;&c_o;&c_s;&c_t;&c_mol;</ind></trd>
</drv>
</art>
