<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <rad>bilbok</rad>/o
  </kap>
  <drv mrk="bilbok.0o">
    <kap><tld/>o</kap>
    <snc>
      <dif>
        <ref tip="super" cel="lud.0ilo">Ludilo</ref>, konsistanta el
        borita globeto alligita per &scirc;nureto al bastoneto, per kiu
        oni devas pikkapti la globeton.  
      </dif>
      <trd lng="be">&c_b;&c_ib;&c_l;&c_mol;&c_b;&c_o;&c_k;&c_e;</trd>
      <trdgrp lng="de">
        <trd>Fang den Ball Spiel</trd>,
        <trd>Bilboquet</trd>
      </trdgrp>
      <trd lng="en">cup-and-ball game</trd>
      <trd lng="fr">bilboquet</trd>
      <trd lng="hu">bilboquet<klr tip="amb"> (goly&oacute;fog&oacute;
      j&aacute;t&eacute;k)</klr></trd>
      <trdgrp lng="nl">
        <trd>bilboquet</trd>,
        <trd>balvangertje</trd>
      </trdgrp>
      <trd lng="pl">bilbokiet</trd>
      <trd lng="ru">&c_b;&c_i;&c_l;&c_mol;&c_b;&c_o;&c_k;&c_je;</trd>
    </snc>
  </drv>
</art>
