<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kuri1.xml,v 1.4 2025/03/06 06:08:26 afido Exp $">
<kap>
    <rad>kuri</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="kuri1.0o">
  <kap><tld/>o</kap>
  <mlg>Ci</mlg>
  <snc mrk="kuri1.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Eksa <ref tip="lst" lst="voko:mezurunuoj" cel="unu.mezur0o.SCI">mezurunuo</ref>
      de radioaktiveco valoranta 3,7 &#xD7; 10<sup>10</sup>
      <ref cel="bekerel.0o.MEZ">bekerelojn</ref>:
      <ekz>
        radioaktiva&jcirc;o je 50 milionoj da <tld/>oj
        <fnt>
        <bib>HomojMan&gcirc;antaj</bib>,
        <lok>&leftquot;La malpermesita arbaro&rightquot;, p. 186a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="kuri.0o">kurio (grupo)</ref>
  <trd lng="be">&c_k;&c_ju;&c_r;&c_y; <klr>(&c_a;&c_d;&c_z;&c_ib;&c_n;&c_k;&c_a; &c_v;&c_y;&c_m;&c_ja;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>
  <trd lng="de">Curie <klr>(Einheit)</klr></trd>
  <trd lng="fr">curie<klr tip="amb"> (unit&eacute; de mesure)</klr></trd>
  <trd lng="en">curie <klr>(unit)</klr></trd>
</drv>
</art>

