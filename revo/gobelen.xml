<vortaro>
<art mrk="$Id: gobelen.xml,v 1.16 2023/11/11 11:36:36 revo Exp $">
<kap>
  <rad>gobelen</rad>/o,
  <var><kap><rad var="i">gobelin</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>
<drv mrk="gobelen.0o">
  <kap><tld/>o, <var><kap><tld var="i"/>o</kap></var></kap>
  <snc mrk="gobelen.0o.TEKS">
    <uzo tip="fak">BELA</uzo>
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Arta mur<ref tip="super" cel="tapisx.0o.TEKS">tapi&scirc;o</ref>:
      <ekz>
        sur molaj hara&jcirc;aj <tld/>oj oni batalas, pafas antikvaj
        kanonoj
        <fnt><bib>Metrop</bib></fnt>;
      </ekz>   
      <ekz>
        &ccirc;i tie estis la landa trezorejo, ri&ccirc;a biblioteko kaj
        kolektoj de armiloj,
        bildoj, juveloj kaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2015/011148.php">Regnestra Palaco en Vilniuso</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        ene de la Parlamentejo multaj pentra&jcirc;oj, statuoj reliefoj
        <tld var="i"/>oj
        ornamas la <klr>[&#x2026;]</klr> salonojn
        <fnt>
        <vrk><url ref="http://www.hungario.hu/bp.html">Budapest</url></vrk>,
        <lok>[vidita je 2006-04-14]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_b;&c_e;&c_l;&c_je;&c_n;</trd>
  <trd lng="ca">tap&iacute;s</trd>
  <trdgrp lng="cs">
    <trd>gobel&iacute;n</trd>,
    <trd>gobl&eacute;n</trd>
  </trdgrp>
  <trd lng="de">Gobelin</trd>
  <trd lng="fr">tapisserie</trd>
  <trd lng="hu">gobelin</trd>
  <trd lng="nl">gobelin</trd>
  <trd lng="pt">gobelim</trd>
  <trd lng="ru">&c_g;&c_o;&c_b;&c_je;&c_l;&c_je;&c_n;</trd>
  <trd lng="sk">gobel&iacute;n</trd>
  <trdgrp lng="tr">
    <trd>hal&inodot;c&inodot;l&inodot;k</trd>,
    <trd>kilimcilik</trd>,
    <trd>duvar ka&gbreve;&inodot;d&inodot;</trd>,
    <trd>duvar hal&inodot;s&inodot;</trd>,
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6302;&#x6BE1; <pr>gu&#xE0;zh&#x101;n</pr></trd>,
    <trd>&#x7E54;&#x9326;&#x756B; <pr>zh&#x12B;j&#x1D0;nhu&#xE0;</pr></trd>,
    <trd>&#x7E61;&#x5E37; <pr>xi&#xF9;w&#xE9;i</pr></trd>,
    <trd>&#x6302;&#x6BE1; <pr>gu&#xE0;zh&#x101;n</pr></trd>,
    <trd>&#x7E54;&#x9326;&#x756B; <pr>zh&#x12B;j&#x1D0;nhu&#xE0;</pr></trd>,
    <trd>&#x7E61;&#x5E37; <pr>xi&#xF9;w&#xE9;i</pr></trd>
  </trdgrp>
</drv>
</art>

