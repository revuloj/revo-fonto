<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kalci.xml,v 1.34 2023/12/02 21:20:04 revo Exp $">
<kap>
   <ofc>9</ofc><rad>kalci</rad>/o 
   <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kalci.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">KEM</uzo>
   <mlg kod="IUPAC">Ca</mlg>
   <snc>
     <dif>
       &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="20">elemento</ref>
       kun atomnumero 20, atompezo 40,08;
       <ref tip="super" cel="metal.teralkala0o">teralkala metalo</ref>
       flave blanka, kiun enhavas <ref cel="kalk.0o">kalko</ref>.
      <ekz>
        bovina lakto estas la plej bona fonto por <tld/>o, bezonata de la korpo
        <fnt>
       <aut>Mu Binghua</aut>
          <vrk><url ref="&Monato;/2000/007169.php">Lakto ne &scirc;atata</url></vrk>
          <bib>Monato</bib>
          <lok>2000/12, p. 11</lok>
        </fnt>;
      </ekz>
      <ekz>
        100 gramoj da karobo entenas 350 miligramojn da <tld/>o, dum la sama kvanto de lakto enhavas
        nur 120
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2011/010535.php">Sanige ... kaj sensukere</url></vrk>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_mol;&c_c;&c_y;&c_j;</trd>
  <trd lng="bg">&c_k;&c_a;&c_l;&c_c;&c_i;&c_j;</trd>
  <trd lng="br">kalsiom</trd>
  <trd lng="ca">calci</trd>
  <trdgrp lng="cs">
    <trd>kalcium</trd>,
    <trd>v&aacute;pn&iacute;k</trd>
  </trdgrp>
  <trd lng="de">Kalzium</trd>
  <trd lng="en">calcium</trd>
  <trd lng="es">calcio</trd>
  <trd lng="fa">&f_k;&a_l;&a_s;&f_y;&a_m;</trd>
  <trd lng="fi">kalsium</trd>
  <trd lng="fr">calcium</trd>
  <trd lng="hu">kalcium</trd>
  <trd lng="id">kalsium</trd>
  <trd lng="ja">&#x30AB;&#x30EB;&#x30B7;&#x30A6;&#x30E0;</trd>
  <trd lng="la">Calcium</trd>
  <trd lng="nl">calcium</trd>
  <trd lng="pl">wap&nacute;</trd>
  <trd lng="pt">c&aacute;lcio</trd>
  <trd lng="ru">&c_k;&c_a;&c_l;&c_mol;&c_c;&c_i;&c_j;</trd>
  <trd lng="sk">v&aacute;pnik</trd>
  <trd lng="sv">kalcium</trd>
  <trd lng="uk">&#x43A;&c_a;&#x43B;&#x44C;&c_c;&c_ib;&c_j;</trd>
  <trd lng="zh">&#x9223; <pr>g&#xE0;i</pr></trd>
</drv>
</art>
