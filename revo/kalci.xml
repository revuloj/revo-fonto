<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kalci</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kalci.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref>
      kun atomnumero 20, atompezo 40,08, simbolo Ca.
      Metalo flaveblanka, kiun enhavas <ref cel="kalk.0o">kalko</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_mol;&c_c;&c_y;&c_j;</trd>
  <trd lng="de">Kalzium</trd>
  <trd lng="en">calcium</trd>
  <trd lng="fr">calcium</trd>
  <trd lng="es">calcio</trd>
  <trd lng="hu">kalcium</trd>
  <trd lng="nl">calcium</trd>
  <trd lng="ru">&c_k;&c_a;&c_l;&c_mol;&c_c;&c_i;&c_j;</trd>
</drv>
</art>
