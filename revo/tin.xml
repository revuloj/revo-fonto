<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tin.xml,v 1.21 2023/10/21 13:52:03 revo Exp $">
<kap>
  <rad>tin</rad>/o
  <fnt><bib>MT</bib></fnt>
</kap>
<drv mrk="tin.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Granda nekovrita <ref tip="super" cel="kuv.0o">kuvo</ref>
      el ligno:
      <ekz>
        en tiu angulo staris <tld/>o por akvo
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o kun butero
        <fnt>
          <bib>Fab3</bib>
          <lok>Koboldo &ccirc;e la butikisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        lesiv<tld/>o, vin<tld/>o;
      </ekz>
      <ekz>
        la premejoj estas plenaj kaj la <tld/>oj
        tropleni&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>Joel 3:13</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    Oni nomas anka&ubreve; <ref tip="hom" cel="tinus.0o">tinuso</ref>n foje
    <tld/>o.
  </rim>
  <trdgrp lng="be">
      <trd>&c_ch;&c_o;&c_p;</trd>,
      <trd>&c_k;&c_a;&c_d;&c_k;&c_a;</trd>,
      <trd>&c_k;&c_a;&c_d;&c_z;&c_mol;</trd>,
      <trd>&c_b;&c_a;&c_l;&c_je;&c_ja;</trd>,
      <trd>&c_b;&c_o;&c_d;&c_n;&c_ja;</trd>,
      <trd>&c_c;&c_e;&c_b;&c_a;&c_r;</trd>,
      <trd>&c_k;&c_u;&c_b;&c_je;&c_l;</trd>
    </trdgrp>
  <trdgrp lng="cs">
    <trd>d&zcaron;ber</trd>,
    <trd>k&aacute;&dcaron;</trd>,
    <trd>putna</trd>,
    <trd>&scaron;kopek</trd>,
    <trd>&scaron;toudev</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>K&uuml;bel</trd>,
      <trd>Trog</trd>,
      <trd>Tonne</trd>
    </trdgrp>
  <trdgrp lng="en">
    <trd>tub</trd>,
    <trd>vat</trd>
  </trdgrp>
  <trdgrp lng="fr">
      <trd>baquet</trd>,
      <trd>auge</trd>,
      <trd>cuve</trd>
    </trdgrp>
  <trdgrp lng="hu">
    <trd>d&eacute;zsa</trd>,
    <trd>sajt&aacute;r</trd>,
    <trd>&uuml;l&odblac;k&aacute;d</trd>
  </trdgrp>
  <trd lng="ja">&#x6876;  <pr>&#x304A;&#x3051;</pr></trd>
  <trdgrp lng="nl">
    <trd>tob</trd>,
    <trd>tobbe</trd>,
    <trd>trog</trd>
  </trdgrp>
  <trdgrp lng="pl">
      <trd>ceber</trd>,
      <trd>kad&zacute;</trd>,
      <trd>szaflik</trd>
    </trdgrp>
  <trdgrp lng="pt">
      <trd>tina</trd>
    </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_a;&c_n;</trd>,
    <trd>&c_b;&c_a;&c_d;&c_mol;&c_ja;</trd>,
    <trd>&c_l;&c_o;&c_h;&c_a;&c_n;&c_mol;</trd>,
    <trd>&c_u;&c_sh;&c_a;&c_t;</trd>,
    <trd>&c_k;&c_a;&c_d;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>d&zcaron;ber</trd>,
    <trd>ka&dcaron;a</trd>,
    <trd>put&ncaron;a</trd>,
    <trd>&scaron;kop&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ch;&c_a;&c_n;</trd>,
    <trd>&c_b;&c_a;&c_l;&c_ib;&c_ja;</trd>,
    <trd>&c_c;&c_je;&c_b;&c_je;&c_r;</trd>,
    <trd>&c_b;&c_a;&c_d;&c_d;&c_ja;</trd>,
    <trd>&c_d;&c_ib;&c_zh;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_a;&c_p;&c_l;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
