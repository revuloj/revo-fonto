<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>panoram</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="panora.0o">
  <kap><tld/>o</kap>
  <snc mrk="panora.0o.vidajxo">
    <dif>
      Vasta vida&jcirc;o &gcirc;uata de alta loko:
      <ekz>
        la <tld/>o de la Alpoj;
      </ekz>
      <ekz>
        vasta <tld/>o prezenti&gcirc;is al ili
        <fnt><bib>Fab2</bib>, <lok>pa&scirc;tistino kaj
        kamentubisto</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="horizo.0o">horizonto</ref>
    </refgrp>
    <trdgrp lng="fa">
      <trd>&f_ch;&a_sh;&a_m;&zwnj;&a_A;&a_n;&a_d;&a_A;&a_z;</trd>,
      <trd>&a_d;&a_w;&a_r;&a_n;&a_m;&a_A;</trd>,
      <trd>&a_m;&a_n;&a_Z;&a_r;&a_h;</trd>
    </trdgrp>
    <trd lng="hu">panor&aacute;ma</trd>
  </snc>
  <snc mrk="panora.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Pentra&jcirc;o sur ronda muro, kiu donas al rigardantoj lokitaj en
      centro
      kvaza&ubreve; vidon al objektoj &ccirc;irka&ubreve;antaj ilin.
    </dif>
    <trd lng="fa">&f_p;&a_A;&a_n;&a_w;&a_r;&a_A;&a_m;&a_A;</trd>
    <trd lng="hu">k&ouml;rk&eacute;p</trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_n;&c_a;&c_r;&c_a;&c_m;&c_a;</trd>
  <trd lng="ca">panorama</trd>
  <trd lng="de">Panorama</trd>
  <trd lng="en">panorama</trd>
  <trdgrp lng="fr">
    <trd>panorama</trd>
  </trdgrp>
  <trd lng="nl">panorama</trd>
  <trd lng="pl">panorama</trd>
  <trd lng="pt">panorama</trd>
  <trd lng="ru">&c_p;&c_a;&c_n;&c_o;&c_r;&c_a;&c_m;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>pohľad</trd>,
    <trd>rozhľad</trd>
  </trdgrp>
</drv>

<drv mrk="panora.0i">
  <kap>panorami</kap>
  <snc mrk="panora.0i.rigardi">
    <dif>
      <tld lit="P"/>e rigardi:
      <ekz>
        oni povas kaj gustumi la &ccirc;inajn pladojn
        kaj <tld/>i la riveron
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url
          ref="http://esperanto.cri.cn/802/2010/02/02/1s108809.htm">
          Parko de la Monda Ekspozicio</url></vrk>,
          <lok>2010-02-02</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">veure panor&agrave;micament</trd>
    <trd lng="fr">embrasser du regard</trd>
  </snc>
</drv>
</art>
