<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  -<rad>&ccirc;j</rad> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="cxj.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc mrk="cxj.0.KOMUNE">
    <dif>
      Karesa sufikso almetata post la la unua a&ubreve; unuaj silaboj 
      de vira nomo:
      <ekz>
        <ind>se Pe<tld/>o ne semos, Petro ne rikoltos</ind>
        <trd lng="hu">ha Jancsi nem vet, J&aacute;nos nem arat</trd>
        <fnt><bib>PrV</bib></fnt>
      </ekz>
      <ekz>
        Nikolao, Nikol<tld/>o, Nik<tld/>o, Ni<tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">-ito<klr> (diminutivo masculino)</klr></trd>
  <trdgrp lng="fr">
    <trd>-ot<klr> (diminutif masculin)</klr></trd>,
    <trd>-ou<klr> (diminutif masculin)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>-<ind>ka/ke</ind><klr> (f&eacute;rfin&eacute;v
    kicsiny&iacute;t&odblac;k&eacute;pz&odblac;)</klr></trd>,
    <trd>-<ind>cska/cske</ind><klr> (f&eacute;rfin&eacute;v
    kicsiny&iacute;t&odblac;k&eacute;pz&odblac;)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>-ito<klr> (diminutivo masculino)</klr></trd>,
    <trd>-inho<klr> (diminutivo masculino)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>-<ind>&c_k;&c_a;</ind><klr tip="ind"> 
(&c_l;&c_a;&c_s;&c_k;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;
    &c_s;&c_u;&c_f;&c_f;&c_i;&c_k;&c_s; 
&c_m;&c_u;&c_zh;&c_s;&c_k;&c_o;&c_g;&c_o; &c_r;&c_o;&c_d;&c_a;)</klr></trd>,
    <trd>-<ind>&c_ch;&c_k;&c_a;</ind><klr tip="ind"> 
(&c_l;&c_a;&c_s;&c_k;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;
    &c_s;&c_u;&c_f;&c_f;&c_i;&c_k;&c_s; 
&c_m;&c_u;&c_zh;&c_s;&c_k;&c_o;&c_g;&c_o; &c_r;&c_o;&c_d;&c_a;)</klr></trd>,
    <trd>-<ind>&c_n;&c_mol;&c_k;&c_a;</ind><klr tip="amb"> 
(&c_l;&c_a;&c_s;&c_k;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;
    &c_s;&c_u;&c_f;&c_f;&c_i;&c_k;&c_s; 
&c_m;&c_u;&c_zh;&c_s;&c_k;&c_o;&c_g;&c_o; &c_r;&c_o;&c_d;&c_a;)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>-ino<klr> (diminutivo maschile)</klr></trd>,
    <trd>-etto<klr> (diminutivo maschile)</klr></trd>,
    <trd>-uccio<klr> (vezzeggiativo maschile)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="cxj.av0o">
  <kap>av<tld/>o</kap>
  <snc mrk="cxj.av0o.KOMUNE">
    <dif>
      Karesvorto por <ref tip="sin" cel="av.0o">avo</ref>:
      <ekz>
        kiam av<tld/>o ankora&ubreve; lo&gcirc;is en South Carolina
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>unua libro, &ccirc;apitro
        6a</lok></fnt>...
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Opa</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>grand-papa</trd>,
    <trd>p&eacute;p&eacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_je;&c_d;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trdgrp lng="it">
    <trd>nonnino</trd>,
    <trd>nonnetto<klr> (ironico)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="cxj.o0o">
  <kap>o<tld/>o</kap>
  <snc mrk="cxj.o0o.KOMUNE">
    <dif>
      Karesvorto por <ref tip="sin" cel="onkl.0o">onklo</ref>.
    </dif>
  </snc>
  <trd lng="fr">tonton</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_ja;&c_d;&c_ja;</trd>,
    <trd>&c_d;&c_ja;&c_d;&c_je;&c_n;&c_mol;&c_k;&c_a;</trd>,
    <trd>&c_d;&c_ja;&c_d;&c_ju;&c_sh;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="it">zietto</trd>
</drv>

<drv mrk="cxj.pa0o">
  <kap>pa<tld/>o</kap>
  <snc mrk="cxj.pa0o.KOMUNE">
    <dif>
      Karesvorto por <ref tip="sin" cel="patr.0o">patro</ref>:
      <ekz>
        la infano babiladis: &leftquot;Patreto! Pa<tld/>o!&rightquot;
        <fnt><bib>FK</bib>, <lok>4, rakontoj, la forgesita pipo, rakonto
        de A. Edelmann, tradukis I. Kaminski</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Papa</trd>
  </trdgrp>
  <trd lng="fr">papa</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_p;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_p;&c_o;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sv">pappa</trd>
  <trdgrp lng="it">
    <trd>pap&agrave;</trd>,
    <trd>papino</trd>,
    <trd>pa'</trd>,
    <trd>babbo</trd>
  </trdgrp>
</drv>
</art>
