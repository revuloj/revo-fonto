<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  -<rad>&ccirc;j</rad>/ <fnt><bib>PV</bib></fnt>
</kap>
<subart>
<drv mrk="cxj.0">
  <kap>-<tld/></kap>
  <snc>
    <dif>
      Sufikso karesa almetata post la unua a&ubreve; unuaj silaboj 
      de vira nomo:
      <ekz>
        <ind>se Pe<tld/>o ne semos, Petro ne rikoltos</ind>
        <trd lng="hu">ha Jancsi nem vet, J&aacute;nos nem arat</trd>
        <trd lng="pl">czego Ja&sacute; si&eogonek; nie nauczy, tego Jan
        nie b&eogonek;dzie umia&lstroke;</trd>
        <fnt><bib>PrV</bib></fnt>
      </ekz>
      <ekz>
        Nikolao, Nikol<tld/>o, Nik<tld/>o, Ni<tld/>o.
      </ekz>
    </dif>
  <refgrp tip="vid">
    <ref cel="nj.0">nj</ref>,
    <ref cel="et.0.sufikso">et</ref>
  </refgrp>  
  </snc>
  <trd lng="es">-ito<klr> (diminutivo masculino)</klr></trd>
  <trdgrp lng="fr">
    <trd>-ot<klr> (diminutif masculin)</klr></trd>,
    <trd>-ou<klr> (diminutif masculin)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>-<ind>ka/ke</ind><klr> (f&eacute;rfin&eacute;v
    kicsiny&iacute;t&odblac;k&eacute;pz&odblac;)</klr></trd>,
    <trd>-<ind>cska/cske</ind><klr> (f&eacute;rfin&eacute;v
    kicsiny&iacute;t&odblac;k&eacute;pz&odblac;)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>-ino<klr> (diminutivo maschile)</klr></trd>,
    <trd>-etto<klr> (diminutivo maschile)</klr></trd>,
    <trd>-uccio<klr> (vezzeggiativo maschile)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>-tje<klr tip="ind"> (mannelijk diminutiefsuffix)</klr></trd>,
    <trd>-pje<klr tip="ind"> (mannelijk diminutiefsuffix)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>-cio<klr> (zdrobnienie rzecz. rodz.
    m&eogonek;skiego)</klr></trd>,
    <trd>-ek<klr> (zdrobnienie rzecz. rodz.
    m&eogonek;skiego)</klr></trd>,
    <trd>-u&sacute;<klr> (zdrobnienie rzecz. rodz.
    m&eogonek;skiego)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>-ito<klr> (diminutivo masculino)</klr></trd>,
    <trd>-inho<klr> (diminutivo masculino)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>-<ind>&c_k;&c_a;</ind><klr tip="ind">
    (&c_l;&c_a;&c_s;&c_k;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;
    &c_s;&c_u;&c_f;&c_f;&c_i;&c_k;&c_s;
    &c_m;&c_u;&c_zh;&c_s;&c_k;&c_o;&c_g;&c_o;
    &c_r;&c_o;&c_d;&c_a;)</klr></trd>,
    <trd>-<ind>&c_ch;&c_k;&c_a;</ind><klr tip="ind">
    (&c_l;&c_a;&c_s;&c_k;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_s;&c_u;&c_f;&c_f;&c_i;&c_k;&c_s; &c_m;&c_u;&c_zh;&c_s;&c_k;&c_o;&c_g;&c_o;
    &c_r;&c_o;&c_d;&c_a;)</klr></trd>,
    <trd>-<ind>&c_n;&c_mol;&c_k;&c_a;</ind><klr tip="amb">
    (&c_l;&c_a;&c_s;&c_k;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_s;&c_u;&c_f;&c_f;&c_i;&c_k;&c_s; &c_m;&c_u;&c_zh;&c_s;&c_k;&c_o;&c_g;&c_o;
    &c_r;&c_o;&c_d;&c_a;)</klr></trd>
  </trdgrp>
</drv>
</subart>

<subart>
<dif>
Samsignifa memstara vortero.
</dif>
<drv mrk="cxj.av0o">
  <kap>av<tld/>o</kap>
  <snc mrk="cxj.av0o.KOMUNE">
    <dif>
      Karesvorto por <ref tip="sin" cel="av.0o">avo</ref>:
      <ekz>
        kiam av<tld/>o ankora&ubreve; lo&gcirc;is en South Carolina
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>unua libro, &ccirc;apitro
        6a</lok></fnt>...
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_d;&c_ja;&c_d;&c_o;</trd>
  <trd lng="de"> Opa </trd>
  <trd lng="en">grandpa</trd>
  <trdgrp lng="fr">
    <trd>papi</trd>,
    <trd>papy</trd>,
    <trd>p&eacute;p&eacute;</trd>,
    <trd>grand-papa</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>nonnino</trd>,
    <trd>nonnetto<klr> (ironico)</klr></trd>
  </trdgrp>
  <trd lng="nl">opa</trd>
  <trdgrp lng="pl">
    <trd>dziadziu&sacute;</trd>,
    <trd>dziadzio</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_je;&c_d;&c_u;&c_sh;&c_k;&c_a;</trd>
</drv>

<drv mrk="cxj.o0o">
  <kap>o<tld/>o</kap>
  <snc mrk="cxj.o0o.KOMUNE">
    <dif>
      Karesvorto por <ref tip="sin" cel="onkl.0o">onklo</ref>.
    </dif>
  </snc>
  <trd lng="fr">tonton</trd>
  <trd lng="it">zietto</trd>
  <trdgrp lng="pl">
    <trd>wujek</trd>,
    <trd>wujcio</trd>,
    <trd>stryjek</trd>,
    <trd>stryjcio</trd>
  </trdgrp>
  <trd lng="nl">oompje</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_ja;&c_d;&c_ja;</trd>,
    <trd>&c_d;&c_ja;&c_d;&c_je;&c_n;&c_mol;&c_k;&c_a;</trd>,
    <trd>&c_d;&c_ja;&c_d;&c_ju;&c_sh;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="cxj.pa0o">
  <kap>pa<tld/>o</kap>
  <snc mrk="cxj.pa0o.KOMUNE">
    <dif>
      Karesvorto por <ref tip="sin" cel="patr.0o">patro</ref>:
      <ekz>
        la infano babiladis: &leftquot;Patreto! Pa<tld/>o!&rightquot;
        <fnt><bib>FK</bib>, <lok>la forgesita pipo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_t;&c_a;&c_t;&c_k;&c_o;</trd>
  <trdgrp lng="de">
    <trd>Papa</trd>
  </trdgrp>
  <trd lng="fr">papa</trd>
  <trdgrp lng="it">
    <trd>pap&agrave;</trd>,
    <trd>papino</trd>,
    <trd>pa'</trd>,
    <trd>babbo</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>papa</trd>,
    <trd>papaatje</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>tata</trd>,
    <trd>tato</trd>,
    <trd>tatu&sacute;</trd>,
    <trd>tatko</trd>,
    <trd>papa</trd>,
    <trd>papcio</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_p;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_p;&c_o;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sv">pappa</trd>
</drv>

<drv mrk="cxj.fra0o">
  <kap>fra<tld/>o</kap>
  <snc mrk="cxj.fra0o.KOMUNE">
    <dif>
      Karesvorto por <ref tip="sin" cel="frat.0o">frato</ref>.
    </dif>
  </snc>
  <trd lng="fr">fr&eacute;rot</trd>
  <trd lng="nl">broertje</trd>
  <trd lng="pl">braciszek</trd>
</drv>

<drv mrk="cxj.fil0o">
  <kap>fil<tld/>o</kap>
  <snc mrk="cxj.fil0o.KOMUNE">
    <dif>
      Karesvorto por <ref tip="sin" cel="fil.0o">filo</ref>.
    </dif>
  </snc>
  <trd lng="fr">fiston</trd>
  <trd lng="nl">jongen<klr tip="ind"> (=zoon)</klr></trd>
  <trdgrp lng="pl">
    <trd>synek</trd>,
    <trd>synu&sacute;</trd>
  </trdgrp>
</drv>
</subart>
</art>
