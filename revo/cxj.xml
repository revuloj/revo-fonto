        patro, pa&ccirc;jo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>-ot<klr> (diminutif masculin)</klr></trd>,
    <trd>-ou<klr> (diminutif masculin)</klr></trd>
  </trdgrp>
