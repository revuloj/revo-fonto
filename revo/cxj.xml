<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  -<rad>&ccirc;j</rad>
</kap>
<drv mrk="cxj.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc mrk="cxj.0.KOMUNE">
    <dif>
      Karesa sufikso almetata post la la unua a&ubreve; unuaj silaboj de vira
      nomo:
      <ekz>
        <ind>se Pe&ccirc;jo ne semos, Petro ne rikoltos</ind>
        <fnt><bib>PrV</bib></fnt>
      </ekz>
      <trd lng="hu">ha Jancsi nem vet, J&aacute;nos nem arat</trd>
      <ekz>
        Nikolao, Nikol&ccirc;jo, Nik&ccirc;jo, Ni&ccirc;jo.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">-ito<klr> (diminutivo masculino)</klr></trd>
  <trdgrp lng="fr">
    <trd>-ot<klr> (diminutif masculin)</klr></trd>,
    <trd>-ou<klr> (diminutif masculin)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>-<ind>ka/ke</ind><klr> (f&eacute;rfin&eacute;v kicsiny&iacute;t&odblac;k&eacute;pz&odblac;)</klr></trd>,
    <trd>-<ind>cska/cske</ind><klr> (f&eacute;rfin&eacute;v kicsiny&iacute;t&odblac;k&eacute;pz&odblac;)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>-ito<klr> (diminutivo masculino)</klr></trd>,
    <trd>-inho<klr> (diminutivo masculino)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="cxj.av0o">
  <kap>av<tld/>o</kap>
  <snc mrk="cxj.av0o.KOMUNE">
    <dif>
      Karesvorto por <ref tip="sin" cel="av.0o">avo</ref>:
      <ekz>
        kiam av<tld/>o ankora&ubreve; lo&gcirc;is en South Carolina
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>unua libro, &ccirc;apitro
        6a</lok></fnt>...
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>grand-papa</trd>,
    <trd>p&eacute;p&eacute;</trd>
  </trdgrp>
</drv>

<drv mrk="cxj.o0o">
  <kap>o<tld/>o</kap>
  <snc mrk="cxj.o0o.KOMUNE">
    <dif>
      Karesvorto por <ref tip="sin" cel="onkl.0o">onklo</ref>.
    </dif>
  </snc>
  <trd lng="fr">tonton</trd>
</drv>

<drv mrk="cxj.pa0o">
  <kap>pa<tld/>o</kap>
  <snc mrk="cxj.pa0o.KOMUNE">
    <dif>
      Karesvorto por <ref tip="sin" cel="patr.0o">patro</ref>:
      <ekz>
        la infano babiladis: &leftquot;Patreto! Pa<tld/>o!&rightquot;
        <fnt><bib>FK</bib>, <lok>4, rakontoj, la forgesita pipo, rakonto
        de A. Edelmann, tradukis I. Kaminski</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">papa</trd>
</drv>
</art>
