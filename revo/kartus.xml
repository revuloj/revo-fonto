<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kartus.xml,v 1.17 2024/04/05 06:26:27 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>kartu&scirc;</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="kartus.0o">
  <kap><tld/>o </kap>
  <snc>
    <dif>
      <ref tip="super" cel="ornam.0o.ajxo">Ornamo</ref>,
      &ccirc;irka&ubreve;anta plata&jcirc;eton, sur kiu &gcirc;enerale
      estas skriba&jcirc;o, nomo, titolo, devizo, insigno:
      <ekz>
        skulptitaj enkadra&jcirc;oj, plafonaj <tld/>oj
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la antikva ceremonia &ccirc;arego, montranta sur siaj <tld/>oj la blazonon de
        Prelongoj, transiris la grandan kradon de la kastelo
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekunua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;efa portalo estas borderita per duonkolonoj kun la dato 1675
        en <tld/>o kun episkopa skulptita portreto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1297">Flor&gcirc;ardeno de
          Krom&ecaron;&rcaron;&iacute;&zcaron;</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="border.0ajxo">bordera&jcirc;o</ref>,
      <ref cel="kadr.0o.borderajxo">kadro<sncref/></ref>
    </refgrp>
    <bld
      lok="&WCU;/thumb/a/a1/Schloss_Weilerbach_19.jpg/320px-Schloss_Weilerbach_19.jpg"
      prm="CC BY-SA 4.0"
      ><tld/>o super pordo de kastelo Weilerbach
        <fnt>
          <aut>Thomas Hummel, propra verko</aut>
          <lok><url ref="&WCW;/File:Schloss_Weilerbach_19.jpg"
              >https://commons.wikimedia.org/wiki/File:Schloss_Weilerbach_19.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_t;&c_u;&c_sh;</trd>
  <trdgrp lng="de">
    <trd>Kartusche <klr>(Zierrahmen)</klr></trd>,
    <trd>Zierrahmen</trd>,
    <trd>Sch&ouml;nleiste</trd>
  </trdgrp>
  <trd lng="en">cartouche</trd>
  <trd lng="fr">cartouche<klr tip="ind"> (subst. masc.)</klr></trd>
  <trdgrp lng="hu">
    <trd>pajzsd&iacute;sz</trd>,
    <trd>k&aacute;rtus</trd>
  </trdgrp>
  <trd lng="ja">&#12459;&#12523;&#12488;&#12453;&#12540;&#12471;&#12517;</trd>
  <trd lng="nl">cartouche</trd>
  <trd lng="pl">kartusz <klr>(hieroglif)</klr></trd>
  <trd lng="ro">emblem&abreve;</trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_t;&c_u;&c_sh;</trd>
  <trd lng="uk">&c_k;&c_a;&c_r;&c_t;&c_u;&c_sh;</trd>
</drv>
</art>
