<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kartus.xml,v 1.14 2019/01/19 00:10:15 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>kartu&scirc;</rad>/o
</kap>
<drv mrk="kartus.0o">
  <kap><tld/>o </kap>
  <snc>
    <dif>
      <ref tip="super" cel="ornam.0o.ajxo">Ornamo</ref>, 
      &ccirc;irka&ubreve;anta plata&jcirc;eton, sur kiu &gcirc;enerale 
      estas skriba&jcirc;o, nomo, titolo, devizo, insigno:
      <ekz>
        la &ccirc;efa portalo estas borderita per duonkolonoj kun la dato 1675
        en <tld/>o kun episkopa skulptita portreto
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref="https://eo.wikipedia.org/?curid=1297">Flor&gcirc;ardeno de
          Krom&ecaron;&rcaron;&iacute;&zcaron;</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_t;&c_u;&c_sh;</trd>
  <trd lng="de">Kartusche <klr>(Zierrahmen)</klr></trd>
  <trd lng="en">cartouche</trd>
  <trd lng="fr">cartouche<klr tip="ind"> (subst. masc.)</klr></trd>
  <trdgrp lng="hu">
    <trd>pajzsd&iacute;sz</trd>,
    <trd>k&aacute;rtus</trd>
  </trdgrp>
  <trd lng="nl">cartouche</trd>
  <trd lng="pl">kartusz <klr>(hieroglif)</klr></trd>     
  <trd lng="ro">emblem&abreve;</trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_t;&c_u;&c_sh;</trd>
</drv>
</art>
