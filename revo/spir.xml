<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
       <ofc>*</ofc>
       <rad>spir</rad>/i
</kap>
<drv mrk="spir.0i">
       <kap><ofc>*</ofc><tld/>i</kap>
       <snc>
         <gra><vspec>ntr</vspec></gra>
         <dif>
           Enlasi aeron en la pulmojn kaj ellasi &gcirc;in post kiam
&gcirc;i reoksigenigis
           la sangon:
           <ekz>
             permesu al mi ek<tld/>i<fnt>B</fnt>
             <klr>(iomete halti)</klr>.
           </ekz>
         </dif>
       </snc>
       <snc>
         <dif>
           = el<tld/>i 2:
           <ekz>
             la &ccirc;ambro <tld/>as la odoron de rozoj<fnt>B</fnt>.
           </ekz>
         </dif>
       </snc>
       <snc>
         <uzo tip="stl">FIG</uzo>
         <dif>
           = el<tld/>i 3:
           <ekz>
             Sa&ubreve;lo <tld/>ante minacojn kaj mortigon
             kontra&ubreve; la dis&ccirc;iploj;
           </ekz>
           <ekz>
             &ccirc;iuj liaj versa&jcirc;oj <tld/>as malgajecon;
           </ekz>
           <ekz>
             &ccirc;iuj verkoj, vortoj kaj agoj de Z <tld/>as tiun
             ideon<fnt>Z</fnt>.
           </ekz>
         </dif>
       </snc>
       <trd lng="de">atmen</trd>
       <trd lng="en">breathe</trd>
       <trd lng="fr">respirer</trd>
       <trdgrp lng="hu">
         <trd>l&eacute;legzik</trd>,
         <trd>lehel</trd>
       </trdgrp>
       <trd lng="nl">ademen</trd>
       <trd lng="ru">&c_d;&c_y;&c_sh;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="spir.0o">
       <kap><tld/>o</kap>
       <snc>
         <dif>
           &Ccirc;iu unufoja <tld/>ago:
           <ekz>
             Dio enblovis en lian nazon <tld/>on de vivo<fnt>Z</fnt>;
           </ekz>
           <ekz>
             fari la lastan <tld/>on<fnt>Z</fnt>
             <klr>(morti)</klr>;
           </ekz>
           <ekz>
             <uzo tip="stl">FIG</uzo>
             <tld/>o de espero blovas tra la mondo;
           </ekz>
           <ekz>
             <uzo tip="stl">FIG</uzo>
             la tutan libron mi legis unu<tld/>e
             <klr>(senhalte)</klr>.
           </ekz>
         </dif>
       </snc>
       <trdgrp lng="fr"><trd>respiration</trd><trd>souffle</trd></trdgrp>
<trdgrp lng="de">
          <trd>Atem</trd>,
          <trd>Odem<klr> [poezie]</klr></trd>,
          <trd>Hauch</trd>
       </trdgrp>
       <trd lng="en">breath</trd>
       <trdgrp lng="hu">
         <trd>l&eacute;legzet</trd>,
         <trd>lehelet</trd>
       </trdgrp>
       <trd lng="nl">adem</trd>
       <trd lng="ru">&c_d;&c_y;&c_h;&c_a;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="spir.0ado">
       <kap><tld/>(ad)o</kap>
       <snc>
         <dif>
           <tld lit="S"/>a funkcio:
           <ekz>
             perdi la <tld/>adon kurante.
           </ekz>
           <trd lng="de">Atmung</trd>
           <trd lng="nl">adem</trd>
         </dif>
       </snc>
       <trd lng="fr">respiration</trd>
       <trd lng="hu">l&eacute;legz&eacute;s</trd>
       <trd lng="ru">&c_d;&c_y;&c_h;&c_a;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="spir.0egi">
       <kap><tld/>egi<fnt>B</fnt></kap>
       <snc>
         <dif>
           Malfacile kaj forte <tld/>i; anheli.
         </dif>
       </snc>
       <trdgrp lng="fr"><trd>&ecirc;tre
<ind>essoufl&eacute;</ind></trd><trd>haleter</trd></trdgrp>
       <trd lng="de">schnaufen</trd>
       <trd lng="hu">liheg</trd>
       <trd lng="nl">hijgen</trd>
       <trd lng="ru">&c_t;&c_ja;&c_zh;&c_je;&c_l;&c_o;
<ind>&c_d;&c_y;&c_sh;&c_a;&c_t;&c_mol;</ind></trd>
</drv>

<drv mrk="spir.el0i">
       <kap>el<tld/>i</kap>
       <gra><vspec>tr</vspec></gra>
       <snc>
         <dif>
           Elpu&scirc;i aeron el la pulmoj.
         </dif>
         <trd lng="de">ausatmen</trd>
         <trdgrp lng="hu">
           <trd>kil&eacute;legzik</trd>,
           <trd>kilehel</trd>
         </trdgrp>
         <trdgrp lng="fr"><trd>expirer</trd><trd>souffler</trd></trdgrp>
         <trd lng="nl">uitademen</trd>
       </snc>
       <snc>
         <dif>
           Eligi odoron, haladzon, a&ubreve; varmon:
           <ekz>
             la sablo el<tld/>is ardantan varmegon<fnt>Z</fnt>.
           </ekz>
         </dif>
         <trd lng="de">ausd&uuml;nsten</trd>
         <trdgrp lng="fr"><trd>exhaler</trd><trd>&eacute;mettre <klr>(une
odeur)</klr></trd></trdgrp>
         <trd lng="hu">&aacute;raszt</trd>
       </snc>
       <snc>
         <uzo tip="stl">FIG</uzo>
         <dif>
           (io) Eksterigi ian econ abstraktan; montri, esprimi; esti
plena je,
           penetrita per:
           <ekz>
             &ccirc;io el<tld/>is sanan gajecon;
           </ekz>
           <ekz>
             la libro el<tld/>as malamon;
           </ekz>
           <ekz>
             atestanto falsama el<tld/>as mensogojn<fnt>Z</fnt>.
           </ekz>
         </dif>
         <refgrp tip="sin">
           <ref cel="odor.0i">odori</ref>.
         </refgrp>
         <trd lng="de">ausstrahlen</trd>
       </snc>
         <trdgrp lng="fr"><trd>respirer</trd><trd>exprimer</trd></trdgrp>
         <trd lng="hu">kisug&aacute;roz</trd>
       <trdgrp lng="ru">
         <trd>&c_v;&c_y;&c_d;&c_y;&c_h;&c_a;&c_t;&c_mol;</trd>,
         <trd>&c_v;&c_y;&c_d;&c_o;&c_h;&c_n;&c_u;&c_t;&c_mol;</trd>
       </trdgrp>
</drv>

<drv mrk="spir.en0i">
       <kap>en<tld/>i</kap>
       <gra><vspec>tr</vspec></gra>
       <snc>
         <dif>
           Enlasi aeron en la pulmojn:
           <ekz>
             li en<tld/>is la parfumajn blovojn, la venton de la erikejo, la
             gudroaromon de la pinoj.
           </ekz>
         </dif>
       </snc>
       <trd lng="de">einatmen</trd>
       <trdgrp lng="fr"><trd>respirer <klr>(un
parfum)</klr></trd><trd>inspirer</trd><trd>aspirer <klr>(par une paille
par
ex.)</klr></trd><trd>inhaler<klr>(m&eacute;dical)</klr></trd></trdgrp>
       <trdgrp lng="hu">
         <trd>bel&eacute;legzik</trd>,
         <trd>belehel</trd>,
         <trd>besz&iacute;v</trd>
       </trdgrp>
       <trd lng="nl">inademen</trd>
       <trdgrp lng="ru">
         <trd>&c_v;&c_d;&c_y;&c_h;&c_a;&c_t;&c_mol;</trd>,
         <trd>&c_v;&c_d;&c_o;&c_h;&c_n;&c_u;&c_t;&c_mol;</trd>
       </trdgrp>
</drv>

<drv mrk="spir.sen0igxi">
       <kap>sen<tld/>i&gcirc;i</kap>
       <snc>
         <dif>
           Perdi la <tld/>on.
         </dif>
       </snc>
       <trd lng="de">au&szlig;er <ind>Atem</ind> kommen</trd>
       <trd lng="hu">kifullad</trd>
       <trdgrp lng="ru">
         <trd>&c_z;&c_a;&c_d;&c_y;&c_h;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,

<trd>&c_z;&c_a;&c_d;&c_o;&c_h;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
         <trd>&c_z;&c_a;&c_p;&c_y;&c_h;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
       </trdgrp>
       <trdgrp lng="fr">
         <trd>s'essoufler</trd>,
         <trd>perdre <ind>haleine</ind></trd>
       </trdgrp>
</drv>

<drv mrk="spir.0blovo">
       <kap><tld/>blovo</kap>
       <uzo tip="fak">FON</uzo>
       <snc>
         <dif>
           Aerkoloneto elpu&scirc;ita el la pulmoj kaj utiligata de la
homo por
           produkti sonojn.
         </dif>
       </snc>
       <trd lng="fr"><ind>colonne</ind> d'air</trd>
       <trd lng="hu">l&eacute;legzet</trd>
</drv>
</art>
