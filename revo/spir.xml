  <gra><vspec>tr</vspec></gra>
  <snc mrk="spir.sen0igi.FIZL">
    <dif>
      Ka&ubreve;zi momentan perdon de la <tld/>o:
      <ekz>
        pezaj botoj trafintaj lian dorson sen<tld/>igis lin
        <fnt>
          <aut>J. Francis</aut>
          <vrk>La Granda Kaldrono</vrk>
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sufok.0i.FIZL">sufoki</ref>
