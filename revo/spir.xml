<drv mrk="spir.en0i">
  <kap>en<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="spir.en0i.FIZL">
    <dif>
      Enlasi aeron en la pulmojn:
      <ekz>
        li en<tld/>is la parfumajn blovojn, la venton de la erikejo, la
        gudroaromon de la pinoj;
      </ekz>
      <ekz>
        &scirc;i en<tld/>adis tiun saman aeron, kiel li
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i klinis unu el la bran&ccirc;oj, por <klr>[&#x2026;]</klr> en<tld/>i ilian
        bonodoron
        <fnt>
          <bib>Fab3</bib>
          <lok>Folio el la &ccirc;ielo</lok>
        </fnt>;
      </ekz>
      <ekz>
        en<tld/>u en vin la tutan fumon
        <fnt>
          <bib>FdO</bib>
        </fnt>;
      </ekz>
      <ekz>
        el<tld/>u kaj en<tld/>u profunde dum unu/du minutoj
        <fnt>
          <bib>Monato</bib>
          <aut>Reza Kheir-khah</aut>
          <vrk><url ref="&Steloj;monato2/007087.html">Son&gcirc;o:
          Mirinda dimensio de nia vivo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="pl">wdycha&cacute;</trd>
  </snc>
  <snc mrk="spir.en0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <klr>(al iu a&ubreve; io)</klr> Animigi:
      <ekz>
        oni forgesis &ccirc;e tio pri la majstro, kiu <klr>[la pupojn]</klr>
        gvidis, kiu en<tld/>is al ili vivon kaj animon
        <fnt>
          <bib>Fab4</bib>
          <lok>Plumo kaj inkujo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Leben einhauchen</trd>
  </snc>
  <trd lng="be">&c_u;&c_d;&c_y;&c_h;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_v;&c_d;&c_i;&c_sh;&c_v;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>nabrat dech</trd>,
    <trd>nadechnout se</trd>,
    <trd>vdechnout do sebe</trd>
  </trdgrp>
  <trd lng="de">einatmen</trd>
  <trdgrp lng="en">
    <trd>inhale</trd>,
    <trd>breathe in</trd>,
    <trd>give life</trd>
  </trdgrp>
  <trd lng="es">inspirar</trd>
  <trdgrp lng="fa">
    <trd>&a_n;&a_f;&a_s;
      &a_f;&a_r;&a_w;&a_b;&a_r;&a_d;&a_n;</trd>
    <trd>&a_d;&a_m;
      &a_f;&a_r;&a_w;&a_b;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>aspirer <klr>(par une paille par ex.)</klr></trd>,
    <trd>inhaler</trd>,
    <trd>inspirer</trd>,
    <trd>respirer <klr>(un parfum)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bel&eacute;legzik</trd>,
    <trd>belehel</trd>,
    <trd>besz&iacute;v</trd>
  </trdgrp>
  <trd lng="nl">inademen</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_d;&c_y;&c_h;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_d;&c_o;&c_h;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nabra&tcaron; dych</trd>,
    <trd>vd&yacute;chnu&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&#x432;&#x434;&#x438;&#x445;&#x430;&#x442;&#x438;</trd>
</drv>

<drv mrk="spir.gxem0i">
