<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: zanzibar.xml,v 1.2 2023/09/05 09:43:57 afido Exp $">
    <kap>
      <rad>Zanzibar</rad>/o
    </kap>

<drv mrk="zanzibar.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="insul.0aro">Insularo</ref>
       en la oriento de Tanzanio kun samnoma &ccirc;efurbo:
      <ekz>
        centroj de sklaveco estis en okcidento, la bordregiono
        de Gvinea Golfo (Angolo)
        kaj en oriento, <tld/>o kaj Mozambiko
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="https://www.monato.be/2004/008160.html">Afriko kaj aidoso</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu festo i&gcirc;is tradicia en Burundo ekde la
        eniro de islamo pere de komercistoj
        el <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>J&eacute;r&eacute;mie Sabiyumva</aut>
          <vrk><url ref="https://www.monato.be/2009/009701.php">Fino de ramadano: &ccirc;u festo de gejunuloj?</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Sansibar</trd>
    <trd lng="en">Zanzibar</trd>
    <trd lng="fr">Zanzibar</trd>
  </snc>
  <trd lng="ja">&#x30B6;&#x30F3;&#x30B8;&#x30D0;&#x30EB;&#x5CF6;  [<ind>&#x30B6;&#x30F3;&#x30B8;&#x30D0;&#x30EB;&#x3057;&#x307E;</ind>]</trd>
</drv>

  </art>
