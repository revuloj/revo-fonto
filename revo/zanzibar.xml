    <kap>
      <rad>Zanzibar</rad>/o
    </kap>

<drv mrk="zanzibar.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="insul.0aro">Insularo</ref>
       en la oriento de Tanzanio kun samnoma &ccirc;efurbo:
      <ekz>
        centroj de sklaveco estis en okcidento, la bordregiono
        de Gvinea Golfo (Angolo)
        kaj en oriento, <tld/>o kaj Mozambiko
        <fnt>

          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2004/008160.html">Afriko kaj aidoso</url></vrk>
          <bib>Monato</bib>
          <lok>2004/02, p. 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu festo i&gcirc;is tradicia en Burundo ekde la
        eniro de islamo pere de komercistoj
        el <tld/>o
        <fnt>

          <aut>J&eacute;r&eacute;mie Sabiyumva</aut>
          <vrk><url ref="&Monato;/2009/009701.php">Fino de ramadano: &ccirc;u festo de
          gejunuloj?</url></vrk>
          <bib>Monato</bib>
          <lok>2009/06, p. 21</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Sansibar</trd>
    <trd lng="en">Zanzibar</trd>
    <trd lng="fr">Zanzibar</trd>
    <trd lng="nl">Zanzibar</trd>
  </snc>
  <trd lng="be">&c_Z;&c_a;&c_n;&c_z;&c_y;&c_b;&c_a;&c_r;</trd>
  <trd lng="ja">&#x30B6;&#x30F3;&#x30B8;&#x30D0;&#x30EB;&#x5CF6;
  <pr>&#x30B6;&#x30F3;&#x30B8;&#x30D0;&#x30EB;&#12375;&#x307E;</pr></trd>
</drv>

