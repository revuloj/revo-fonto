<drv mrk="aviad.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="aviad.0i.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      Aerveturi per tiaj aparatoj;
      <ref tip="sin" cel="avi.0i.AVI">avii</ref>,
      <ref tip="sin" cel="flug.0i.aviadile">flugi<sncref/></ref>:
      <ekz>
        &ccirc;i-jare mi multe <tld/>as <klr>[&#x2026;]</klr> fakte, mi rondiras la terglobon
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2007/009272.php">Malfavore fervoje</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        pli pla&ccirc;as, <tld/>i de konferenco al konferenco ol enkoti&gcirc;i en ekonomiaj krizoj,
        senlaboreco, inflacio
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2011/010345.php">Ne menciu la n-vorton</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x5F00;&#x98DE;&#x673A; <pr>k&#x101;if&#x113;ij&#x12B;</pr></trd>,
    <trd>&#x958B;&#x98DB;&#x6A5F; <pr>k&#x101;if&#x113;ij&#x12B;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_l;&c_ja;&c_c;&c_je;&c_c;&c_mol; <klr tip="ind">(&c_s;&c_a;&c_m;&c_a;&c_l;&c_jo;&c_t;&c_a;&c_m;,
  &c_v;&c_je;&c_r;&c_t;&c_a;&c_l;&c_jo;&c_t;&c_a;&c_m; &c_ib; &c_d;&c_a;
  &c_g;.&c_p;.)</klr></trd>
  <trdgrp lng="de">
    <trd>ein Flugzeug <ind>fliegen</ind></trd>,
    <trd>ein Flugzeug <ind>steuern</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>fly</trd>,
    <trd>aviate</trd>
  </trdgrp>
  <trd lng="es">volar <klr tip="ind">(en avi&oacute;n)</klr></trd>
  <trd lng="fr">voler <klr tip="ind">(en avion)</klr></trd>
  <trd lng="he">&lamed;<ind>&tet;&vav;&samekh;</ind></trd>
  <trd lng="hu">rep&uuml;l <klr tip="ind">(rep&uuml;l&odblac;g&eacute;ppel)</klr></trd>
  <trd lng="io">aviacar</trd>
  <trd lng="it">volare<klr> (con velivolo)</klr></trd>
  <trd lng="ja">&#x98DB;&#x884C;&#x6A5F;&#x3067;&#x98DB;&#x3076; <pr>&#x3072;&#x3053;&#x3046;&#x304D;&#x3067;&#x3068;&#x3076;</pr></trd>
  <trd lng="nl">vliegen <klr tip="ind">(in vliegtuig)</klr></trd>
  <trd lng="pl">lata&cacute;</trd>
  <trd lng="ru">&c_l;&c_je;&c_t;&c_a;&c_t;&c_mol; <klr tip="ind">(&c_n;&c_a;
  &c_s;&c_a;&c_m;&c_o;&c_l;&c_jo;&c_t;&c_je;)</klr></trd>
  <trd lng="sk">letie&tcaron; <klr>(lietadlom)</klr></trd>
  <trd lng="sk">lieta&tcaron; lietadlom</trd>
  <trdgrp lng="cs">
    <trd>let&ecaron;t letadlem</trd>,
    <trd>pilotovat letadlo</trd>
  </trdgrp>
  <trd lng="uk">&c_k;&c_je;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i; &c_l;&c_ib;&c_t;&c_a;&c_k;&c_o;&c_m;</trd>
</drv>

<drv mrk="aviad.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="aviad.0ilo.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      Flugma&scirc;ino pli peza ol la aero, destinita por flugado en
      atmosfero, precipe tiu uzanta motoron kaj flugilojn:
      <ekz>
        tiuj niaj prauloj disponis nek <klr>[&#x2026;]</klr> pri lernejoj, nek ili havis
        telefonon, radion, televidon, &scirc;ipojn, fervojojn, <tld/>ilojn
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la distancon de 3194 km la <tld/>ilo trapasis en 4:10
        horoj
        <fnt>
          <aut>J. de N. Marques</aut>
          <vrk>Voja&gcirc;o al Ushuaia</vrk>
          <bib>Monato</bib>
          <lok>2002:2, p. 18a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <nom>Ilju&scirc;in</nom>-<tld/>ilo forlasis la delhian flughavenon
        <fnt>
          <bib>CKv</bib>
          <lok>1</lok>
        </fnt>;
      </ekz>
      <ekz>
        la stevardino diris, ke biero en la <tld/>ilo ne trovi&gcirc;as
        <fnt>
          <bib>CKv</bib>
          <lok>1</lok>
        </fnt>;
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="aeropl.0o">aeroplano</ref>,
      <ref cel="avi.0o">avio</ref>
    </refgrp>
    <trdgrp lng="zh">
