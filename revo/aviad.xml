<drv mrk="aviad.pasagxer0ilo">
  <kap>pasa&gcirc;er<tld/>ilo</kap>
  <snc mrk="aviad.pasagxer0ilo.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      <ref tip="super" cel="aviad.0ilo.AVI"><tld lit="A"/>ilo</ref>
      portanta homojn, voja&gcirc;antojn:
      <ekz>
        la <tld/>ilfabriko Aerbuso registrigis la patenton de
        supersona pasa&gcirc;er<tld/>ilo, kiu flugos pli rapide,
        ol e&ccirc; la fama Konkordo
        <fnt>
          <aut>Josenilton kaj Madragoa</aut>
