<drv mrk="aviad.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="aviad.0isto.AVI">
    <dif>
      Tiu, kiu funkciigas <tld/>ilon:
      <ekz>
        <tld/>istoj bombardas ruinojn lasitajn de Sovetio
        <fnt>
          <aut>M. Marko</aut>
          <vrk>Sekretaj organizantoj</vrk>
          <bib>Monato</bib>
          <lok>2002:2, p. 6a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <adm>
    "Aviadisto" ne nur veturas per ~ilo, sed stiras
    &gcirc;in. Tial mi &scirc;an&gcirc;is la difinon de "aerveturisto"
    al "tiu, kiu funkciigas". PIV montras la saman sencan
    restrikton por "~i", kio tamen &scirc;ajnas iom troa. Komparu
    "a&ubreve;ti"
    kaj "a&ubreve;tisto".(MB)
  </adm>
  <trdgrp lng="be">
    <trd>&c_l;&c_jo;&c_t;&c_ch;&c_y;&c_k;</trd>,
    <trd>&c_p;&c_ib;&c_l;&c_o;&c_t;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_i;&c_l;&c_o;&c_t;<klr tip="amb"> (&c_n;&c_a;
  &c_s;&c_a;&c_m;&c_o;&c_l;&c_je;&c_t;)</klr></trd>
  <trdgrp lng="ca">
    <trd>aviador</trd>,
    <trd>pilot</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Pilot</trd>,
    <trd>Flieger</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>pilot</trd>,
    <trd>aviator</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>aviador</trd>,
    <trd>piloto</trd>
  </trdgrp>
  <trd lng="fr">aviateur</trd>
  <trdgrp lng="he">
    <trd>&tet;&yod;&yod;&samekh;</trd>,
    <trd>&nun;&vav;&vav;&tet;<klr tip="ind">
    (&bet;&mem;&tet;&vav;&samekh;)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>rep&uuml;l&odblac;</trd>,
    <trd>pil&oacute;ta</trd>
  </trdgrp>
  <trdgrp lng="io">
    <trd>aviacero</trd>,
    <trd>aviacisto</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>aviatore</trd>,
    <trd>pilota<klr> (d'aereo)</klr></trd>
  </trdgrp>
  <trd lng="nl">piloot</trd>
  <trdgrp lng="pl">
    <trd>pilot</trd>,
    <trd>lotnik</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_jo;&c_t;&c_ch;&c_i;&c_k;</trd>,
    <trd>&c_a;&c_v;&c_i;&c_a;&c_t;&c_o;&c_r;</trd>,
    <trd>&c_p;&c_i;&c_l;&c_o;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>letec</trd>,
    <trd>pilot</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>aviatik</trd>,
    <trd>letec</trd>,
    <trd>pilot</trd>
  </trdgrp>
</drv>

<drv mrk="aviad.karg0ilo">
  <kap>karg<tld/>ilo,
  <var><kap>var<tld/>ilo</kap></var>
  </kap>
  <snc mrk="aviad.karg0ilo.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      <ref tip="super" cel="aviad.0ilo.AVI"><tld lit="A"/>ilo</ref>
      portanta kargon de objektoj, de varoj:
      <ekz>
        <klr>[por]</klr> transporti la steleon al Etiopio probable
        oni utiligos grandajn karg<tld/>ilojn
        <fnt>
          <aut>A. De Salvo</aut>
          <vrk><url ref="http://www.bitoteko.it/esperanto-vivo/eo/2018/09/04/obelisco-di-axum/">
		  Obelisko de Aksumo</url></vrk>
          <lok>2018-09-04</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>cargo aircraft</trd>,
    <trd>freight aircraft</trd>,
    <trd>freight carrier</trd>,
    <trd>freighter<klr tip="ind"> (aircraft)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>avion-cargo</trd>,
    <trd>avion de transport</trd>
  </trdgrp>
  <trd lng="ja">&#x8CA8;&#x7269;&#x6A5F; [<ind>&#x304B;&#x3082;&#x3064;&#x304D;</ind>]</trd>
  <trd lng="nl">vrachtvliegtuig</trd>
</drv>

<drv mrk="aviad.milit0ilo">
  <kap>milit<tld/>ilo, <var><kap>milita <tld/>ilo</kap></var></kap>
  <snc mrk="aviad.milit0ilo.AVI">
    <uzo tip="fak">AVI</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="super" cel="aviad.0ilo.AVI"><tld lit="A"/>ilo</ref>
      uzata de <ref cel="arme.aer0o">aerarmeo</ref>:
      <ekz>
        la tekniko, kiu faris la tankojn kaj la milit<tld/>ilojn,
        <klr>[...]</klr> estis tiu sama tekniko, kiu eltrovis novajn
        metodojn por persekuti la homojn pro la raso, al kiu ili
        apartenas,
        la religio, kiun ili kredas, la lingvo, kiun ili parolas
        <fnt>
        <bib>Voja&gcirc;o</bib>
        <aut>H. Tonkin</aut>
        <vrk>Venas nun la periodo de disvastigo</vrk>
        <lok>leciono 26a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">vojensk&eacute; letadlo</trd>
  <trdgrp lng="en">
    <trd>military aircraft</trd>,
    <trd>warplane</trd>
  </trdgrp>
  <trd lng="es">avi&oacute;n militar</trd>
  <trd lng="fr">avion militaire</trd>
  <trdgrp lng="it">
    <trd><ind>aereo</ind> militare</trd>,
    <trd><ind>velivolo</ind> militare</trd>
  </trdgrp>
  <trd lng="pl">samolot wojskowy</trd>
  <trd lng="ru">&c_v;&c_o;&c_je;&c_n;&c_n;&c_y;&c_j;
  <ind>&c_s;&c_a;&c_m;&c_o;&c_l;&c_jo;&c_t;</ind></trd>
  <trd lng="sk">vojensk&eacute; lietadlo</trd>
</drv>

