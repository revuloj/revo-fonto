<drv mrk="aviad.0o">
  <kap><tld/>o</kap>
  <snc mrk="aviad.0o.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      Arto aerveturadi per aparatoj pli pezaj ol la aero:
      <ekz>
        en <tld/>o gravan rolon por sekureco ludas la komunikado inter
        regturo kaj <tld/>ilo
        <fnt>
          <aut>Pejno S.</aut>
          <vrk>Prefere pakete</vrk>
          <bib>Monato</bib>
          <lok>2001:2a, p. 13a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    Kp <ref cel="avi.0i">avii</ref>, kies substantivigo donas
    samforman vorton <ctl>avi/ad/o</ctl>.
    <aut>Sergio Pokrovskij</aut>
  </rim>
  <trd lng="be">&c_a;&c_v;&c_ib;&c_ja;&c_c;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_a;&c_v;&c_i;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trd lng="de">Flugwesen</trd>
  <trd lng="en">aviation</trd>
  <trd lng="es">aviaci&oacute;n</trd>
  <trd lng="fr">aviation</trd>
  <trd lng="ms">penerbangan</trd>
  <trdgrp lng="he">
    <trd>&tav;&ayin;&vav;&pe;&he;</trd>,
    <trd>&tet;&yod;&yod;&samekh;</trd>
  </trdgrp>
  <trd lng="hu">rep&uuml;l&eacute;s</trd>
  <trdgrp lng="io">
    <trd>aviacado</trd>,
    <trd>aviaco</trd>
  </trdgrp>
  <trd lng="it">aviazione</trd>
  <trdgrp lng="ja">
    <trd>&#x822A;&#x7A7A; [<ind>&#x3053;&#x3046;&#x304F;&#x3046;</ind>]</trd>,
    <trd>&#x98DB;&#x884C; [<ind>&#x3072;&#x3053;&#x3046;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">luchtvaart</trd>
  <trd lng="pl">latanie</trd>
  <trd lng="ro">avia&tcedil;ie</trd>
  <trd lng="ru">&c_a;&c_v;&c_i;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sk">lietanie <klr>(lietadlom)</klr></trd>
  <trd lng="sk">letectvo</trd>
  <trdgrp lng="cs">
    <trd>aviatika</trd>,
    <trd>letectv&iacute;</trd>,
    <trd>l&eacute;t&aacute;n&iacute; letadlem</trd>
  </trdgrp>
</drv>

<drv mrk="aviad.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="aviad.0i.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      Aerveturi per tiaj aparatoj;
      <ref tip="sin" cel="avi.0i.AVI">avii</ref>,
      <ref tip="sin" cel="flug.0i.aviadile">flugi<sncref/></ref>.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_c;&c_je;&c_c;&c_mol; <klr tip="ind">(&c_s;&c_a;&c_m;&c_a;&c_l;&c_jo;&c_t;&c_a;&c_m;,
  &c_v;&c_je;&c_r;&c_t;&c_a;&c_l;&c_jo;&c_t;&c_a;&c_m; &c_ib; &c_d;&c_a;
  &c_g;.&c_p;.)</klr></trd>
  <trdgrp lng="de">
    <trd>fliegen</trd>,
    <trd>steuern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>fly</trd>,
    <trd>aviate</trd>
  </trdgrp>
  <trd lng="es">volar <klr tip="ind">(en avi&oacute;n)</klr></trd>
  <trd lng="fr">voler <klr tip="ind">(en avion)</klr></trd>
  <trd lng="he">&lamed;<ind>&tet;&vav;&samekh;</ind></trd>
  <trd lng="hu">rep&uuml;l <klr tip="ind">(rep&uuml;l&odblac;g&eacute;ppel)</klr></trd>
  <trd lng="io">aviacar</trd>
  <trd lng="it">volare<klr> (con velivolo)</klr></trd>
  <trd lng="ja">&#x98DB;&#x884C;&#x6A5F;&#x3067;&#x98DB;&#x3076;</trd>
  <trd lng="nl">vliegen <klr tip="ind">(in vliegtuig)</klr></trd>
  <trd lng="pl">lata&cacute;</trd>
  <trd lng="ru">&c_l;&c_je;&c_t;&c_a;&c_t;&c_mol; <klr tip="ind">(&c_n;&c_a;
  &c_s;&c_a;&c_m;&c_o;&c_l;&c_jo;&c_t;&c_je;)</klr></trd>
  <trd lng="sk">letie&tcaron; <klr>(lietadlom)</klr></trd>
  <trd lng="sk">lieta&tcaron; lietadlom</trd>
  <trdgrp lng="cs">
    <trd>let&ecaron;t letadlem</trd>,
    <trd>pilotovat letadlo</trd>
  </trdgrp>
</drv>

<drv mrk="aviad.0ilo">
