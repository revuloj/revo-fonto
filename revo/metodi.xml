<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap><rad>Metodi</rad>/o</kap>

    <drv mrk="metodi.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">NOM</uzo>
      <snc>
        <dif>
          Vira antaŭnomo grekdevena:
          <ekz>
            ruslandanoj kaj ankaŭ aliaj ĉiujare gloras la inventintojn
            de la slava, tiel nomata cirila, alfabeto kaj ties
            inventintojn la fratojn Cirilon kaj <tld/>on
            <fnt>
              <aut>Aleksandras MASIUKAS</aut>:
              <vrk>Celebradi pri alfabeto</vrk>. <bib>Monato</bib>
            </fnt>.
          </ekz>
        </dif>
        <trd lng="el">Μεθ&omikron_ton;διος</trd>
        <trd lng="en">Methodius</trd>
        <trd lng="pl">Metody</trd>
        <trd lng="pt">Met&oacute;dio</trd>
        <trd lng="ru">Мефодий</trd>
      </snc>
    </drv>

  </art>

