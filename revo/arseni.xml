<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>arsenik</rad>/o
</kap>
<drv mrk="arseni.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Ev.
    </dif>
    <ref tip="sin" cel="arsen.0o">arseno</ref>
  </snc>
  <snc>
    <dif>
      &Ccirc;iu el la venenaj kompona&jcirc;oj de arseno, precipe la arsenita
      anhidrido As<sub>2</sub>O<sub>3</sub>.
    </dif>
  </snc>
  <trd lng="de">Arsenik</trd>
  <trd lng="fr">arsenic (poison)</trd>
  <trd lng="ru">&c_m;&c_y;&c_sh;&c_mol;&c_ja;&c_k; <klr tip="amb">(&c_ja;&c_d;)</klr></trd>
  <trd lng="sv">arsenik</trd>
</drv>
</art>
