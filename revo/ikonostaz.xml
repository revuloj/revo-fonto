<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ikonostaz.xml,v 1.12 2022/04/30 08:42:42 revo Exp $">
<kap>
  <rad>ikonostaz</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="ikonostaz.0o">
  <kap><tld/>o</kap>
  <snc mrk="ikonostaz.0o.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      En ortodoksa <ref tip="malprt" cel="kirk.0o">kirko</ref>,
      vando el <ref cel="ikon.0o">ikonoj</ref> apartiganta la
      <ref cel="altar.0ejo">altarejon</ref>
      disde la meza parto de la kirko:
      <ekz>
        la ora <tld/>o brilis pro lumo de kandeletoj,
        &ccirc;iuflanke &ccirc;irka&ubreve;antaj la kandelegojn
        <fnt>
          <aut>L. Tolstoj, trad. V. Sapo&jcirc;nikov</aut>,
          <vrk><url ref="&Sezonoj;2015/04/tolstoj/">
          Resurekto</url></vrk>,
          <lok>2000</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_k;&c_a;&c_n;&c_a;&c_s;&c_t;&c_a;&c_s;</trd>
  <trd lng="ca">icon&ograve;stasi</trd>
  <trdgrp lng="cs">
    <trd>ikonostasis</trd>,
    <trd>ikonost&#xE1;z</trd>
  </trdgrp>
  <trd lng="en">iconostasis</trd>
  <trd lng="es">iconostasio</trd>
  <trd lng="fr">iconostase</trd>
  <trdgrp lng="hu">
    <trd>ikonoszt&aacute;z</trd>,
    <trd>olt&aacute;rfal</trd>
  </trdgrp>
  <trd lng="pl">ikonostas</trd>
  <trd lng="pt">icon&oacute;stase</trd>
  <trd lng="ro">iconostas</trd>
  <trd lng="ru">&c_i;&c_k;&c_o;&c_n;&c_o;&c_s;&c_t;&c_a;&c_s;</trd>
  <trd lng="sk">ikonost&#xE1;z</trd>
  <trd lng="uk">&c_ib;&#x43A;&#x43E;&#x43D;&#x43E;&c_s;&c_t;&c_a;&c_s;</trd>
</drv>
</art>

