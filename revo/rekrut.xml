<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rekrut.xml,v 1.20 2022/03/26 18:10:08 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>rekrut</rad>/o
</kap>
<drv mrk="rekrut.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">MIL</uzo>
    <dif>
      Nova soldato:
      <ekz>
        instrui la <tld/>ojn.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_v;&c_a;&c_b;&c_r;&c_a;&c_n;&c_je;&c_c;</trd>,
      <trd>&c_r;&c_e;&c_k;&c_r;&c_u;&c_t;</trd>,
      <trd>&c_p;&c_r;&c_y;&c_z;&c_y;&c_w;&c_n;&c_ib;&c_k;</trd>
    </trdgrp>
    <trd lng="de">Rekrut</trd>
    <trd lng="en">recruit</trd>
    <trd lng="fr">recrue</trd>
    <trdgrp lng="hu">
      <trd>&uacute;jonc</trd>,
      <trd>regruta</trd>
    </trdgrp>
    <trd lng="nl">rekruut</trd>
    <trd lng="pl">rekrut</trd>
    <trd lng="pt">recruta</trd>
    <trdgrp lng="ru">
      <trd>&c_n;&c_o;&c_v;&c_o;&c_b;&c_r;&c_a;&c_n;&c_je;&c_c;</trd>,
      <trd>&c_r;&c_je;&c_k;&c_r;&c_u;&c_t;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_z;&c_y;&c_v;&c_n;&c_i;&c_k;</trd>
    </trdgrp>
    <trd lng="sv">rekryt</trd>
  </snc>
  <snc>
    <dif>
      Novevarbito:
      <ekz>
        nia societo bezonas <tld/>ojn.
      </ekz>   
    </dif>
      <trd lng="be">&c_n;&c_a;&c_v;&c_a;&c_b;&c_r;&c_a;&c_n;&c_je;&c_c;</trd>
      <trd lng="hu">&uacute;jonc</trd>  
      <trdgrp lng="nl">
        <trd>nieuweling</trd>,
        <trd>nieuwkomer</trd>
      </trdgrp> 
      <trd lng="pl">rekrut</trd>   
      <trd lng="ru">&c_n;&c_o;&c_v;&c_o;&c_b;&c_r;&c_a;&c_n;&c_je;&c_c;</trd>
      </snc>
  <trd lng="ca">recluta</trd>
  <trdgrp lng="cs">
    <trd>branec</trd>,
    <trd>nov&aacute;&ccaron;ek</trd>,
    <trd>odvedenec</trd>,
    <trd>zast. rekrut</trd>
  </trdgrp>
  <trd lng="ro">recru</trd>
  <trdgrp lng="sk">
    <trd>branec</trd>,
    <trd>nov&aacute;&ccaron;ik</trd>,
    <trd>odvedenec</trd>,
    <trd>regr&uacute;t</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x440;&#x435;&#x43A;&#x440;&#x443;&#x442;</trd>,
    <trd>&#x43F;&#x440;&#x438;&#x437;&#x43E;&#x432;&#x43D;&#x438;&#x43A;</trd>,
    <trd>&#x43D;&#x43E;&#x432;&#x43E;&#x431;&#x440;&#x430;&#x43D;&#x435;&#x446;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="rekrut.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="rekrut.0igi.MIL">
  <uzo tip="fak">MIL</uzo>
    <dif>
       Fari iun <tld/>o:
       <ekz>
         kial vi <tld/>igas lin, se li estas forta kaj sana?
         <fnt>      
           <bib>SatirRak</bib>
         </fnt>
       </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>reclutar</trd>,
      <trd>allistar</trd>
    </trdgrp>
    <trd lng="de">einziehen</trd>
    <trdgrp lng="nl">
      <trd>rekruteren</trd>,
      <trd>aanwerven</trd>,
      <trd>lichten</trd>
    </trdgrp>
  </snc>
  <snc mrk="rekrut.0igi.FIG">
     <uzo tip="stl">FIG</uzo>
     <dif>
       Kolekti, &ccirc;erpi:
      <ekz>
        tiu slango <tld/>igas siajn vortradikojn plejparte 
        el hispana kaj franca lingvoj.
      </ekz> 
      </dif> 
      <trd lng="ca">recollir</trd>
      <trd lng="de">rekrutieren</trd> 
      <trd lng="nl">rekruteren</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>odv&eacute;st na vojnu</trd>,
    <trd>povolat do arm&aacute;dy</trd>,
    <trd>verbovat</trd>
  </trdgrp>
  <trd lng="en">recruit</trd>
  <trd lng="fr">recruter</trd>
  <trdgrp lng="pl">
    <trd>rekrutowa&cacute;</trd>,
    <trd>powo&lstroke;ywa&cacute;</trd>
  </trdgrp>
  <trd lng="ro">recrutare</trd>
  <trdgrp lng="sk">
    <trd>odvies&tcaron; na vojnu</trd>,
    <trd>povola&tcaron; do arm&aacute;dy</trd>,
    <trd>verbova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x440;&#x435;&#x43A;&#x440;&#x443;&#x442;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x440;&#x438;&#x437;&#x438;&#x432;&#x430;&#x442;&#x438; &#x432; &#x430;&#x440;&#x43C;&#x456;&#x44E;</trd>,
    <trd>&#x431;&#x440;&#x430;&#x442;&#x438;/&#x432;&#x437;&#x44F;&#x442;&#x438; &#x434;&#x43E; &#x432;&#x456;&#x439;&#x441;&#x44C;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>

</art>
