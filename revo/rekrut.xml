<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rekrut</rad>/o
</kap>
<drv mrk="rekrut.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Nova soldato:
      <ekz>
        instrui la <tld/>ojn.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_v;&c_a;&c_b;&c_r;&c_a;&c_n;&c_je;&c_c;</trd>,
      <trd>&c_r;&c_e;&c_k;&c_r;&c_u;&c_t;</trd>,
      <trd>&c_p;&c_r;&c_y;&c_z;&c_y;&c_w;&c_n;&c_ib;&c_k;</trd>
    </trdgrp>
    <trd lng="de">Rekrut</trd>
    <trdgrp lng="hu">
      <trd>&uacute;jonc</trd>,
      <trd>regruta</trd>
    </trdgrp>
    <trd lng="nl">rekruut</trd>
    <trd lng="pt">recruta</trd>
    <trdgrp lng="ru">
      <trd>&c_n;&c_o;&c_v;&c_o;&c_b;&c_r;&c_a;&c_n;&c_je;&c_c;</trd>,
      <trd>&c_r;&c_je;&c_k;&c_r;&c_u;&c_t;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_z;&c_y;&c_v;&c_n;&c_i;&c_k;</trd>
    </trdgrp>
    <trd lng="sv">rekryt</trd>
  </snc>
  <snc>
    <dif>
      Novevarbito:
      <ekz>
        nia societo bezonas <tld/>ojn;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu slango <tld/>igas <klr>(&ccirc;erpas, kolektas)</klr>
        siajn vortradikojn plejparte el hispana kaj franca lingvoj.
      </ekz>      
    </dif>
      <trd lng="be">&c_n;&c_a;&c_v;&c_a;&c_b;&c_r;&c_a;&c_n;&c_je;&c_c;</trd>
      <trd lng="en">recruit</trd>
      <trd lng="hu">&uacute;jonc</trd>  
      <trdgrp lng="nl">
        <trd>nieuweling</trd>,
        <trd>nieuwkomer</trd>
      </trdgrp>    
      <trd lng="ru">&c_n;&c_o;&c_v;&c_o;&c_b;&c_r;&c_a;&c_n;&c_je;&c_c;</trd>
      </snc>
</drv>
</art>
