<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rekrut.xml,v 1.27 2024/04/13 20:15:52 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>rekrut</rad>/o
</kap>
<drv mrk="rekrut.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="rekrut.0o.MIL">
  <uzo tip="fak">MIL</uzo>
    <dif>
      Nova <ref tip="super" cel="soldat.0o">soldato</ref>:
      <ekz>
        kaporalo, instruanta <tld/>ojn
        <fnt>
          <bib>SrT</bib>
          <lok>Libro IX.</lok>
        </fnt>;
      </ekz>
      <ekz>
        li mem salutis la <tld/>ojn kaj instigis ilin al diligenteco en la manovroj por
        la pereigo de la malamikoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la litova armeo servas &ccirc;irka&ubreve; 10&#x202F;000 soldatoj, el kiuj la duono estas <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>LAST</aut>
          <vrk><url ref="&Monato;/2003/007662.html">Nesolidaj soldatoj</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_v;&c_a;&c_b;&c_r;&c_a;&c_n;&c_je;&c_c;</trd>,
      <trd>&c_r;&c_e;&c_k;&c_r;&c_u;&c_t;</trd>,
      <trd>&c_p;&c_r;&c_y;&c_z;&c_y;&c_w;&c_n;&c_ib;&c_k;</trd>
    </trdgrp>
    <trd lng="de">Rekrut</trd>
    <trd lng="en">recruit</trd>
    <trd lng="fr">recrue</trd>
    <trdgrp lng="hu">
      <trd>&uacute;jonc</trd>,
      <trd>regruta</trd>
    </trdgrp>
    <trd lng="nl">rekruut</trd>
    <trd lng="pl">rekrut</trd>
    <trd lng="pt">recruta</trd>
    <trdgrp lng="ru">
      <trd>&c_n;&c_o;&c_v;&c_o;&c_b;&c_r;&c_a;&c_n;&c_je;&c_c;</trd>,
      <trd>&c_r;&c_je;&c_k;&c_r;&c_u;&c_t;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_z;&c_y;&c_v;&c_n;&c_i;&c_k;</trd>
    </trdgrp>
    <trd lng="sv">rekryt</trd>
    <trd lng="zh">&#x65B0;&#x5175; <pr>x&#x12B;nb&#x12B;ng</pr></trd>
</snc>
  <snc mrk="rekrut.0o.varbito">
    <dif>
      Nova varbito:
      <ekz>
        <nom>Klem</nom> penis varbi sian unuan <tld/>on por la movado parolante angle al la
        sinjorino
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 6</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi alkondukis ja tutan aregon da <tld/>oj, vi bravega varbisto
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>!
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="nov.0ulo">novulo</ref>,
      <ref cel="prozel.0o">prozelito</ref>
    </refgrp>
    <trd lng="be">&c_n;&c_a;&c_v;&c_a;&c_b;&c_r;&c_a;&c_n;&c_je;&c_c;</trd>
    <trd lng="hu">&uacute;jonc</trd>
    <trdgrp lng="nl">
      <trd>nieuweling</trd>,
      <trd>nieuwkomer</trd>
    </trdgrp>
    <trd lng="pl">rekrut</trd>
    <trd lng="ru">&c_n;&c_o;&c_v;&c_o;&c_b;&c_r;&c_a;&c_n;&c_je;&c_c;</trd>
  </snc>
  <trd lng="ca">recluta</trd>
  <trdgrp lng="cs">
    <trd>branec</trd>,
    <trd>nov&aacute;&ccaron;ek</trd>,
    <trd>odvedenec</trd>,
    <trd>zast. rekrut</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x65B0;&#x5175;  <pr>&#x3057;&#x3093;&#x307A;&#x3044;</pr></trd>,
    <trd>&#x65B0;&#x5165;&#x4F1A;&#x54E1;  <pr>&#x3057;&#x3093;&#x306B;&#x3085;&#x3046;&#x304B;&#x3044;&#x3044;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="ro">recru</trd>
  <trdgrp lng="sk">
    <trd>branec</trd>,
    <trd>nov&aacute;&ccaron;ik</trd>,
    <trd>odvedenec</trd>,
    <trd>regr&uacute;t</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_je;&c_k;&c_r;&c_u;&c_t;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_z;&c_o;&c_v;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_n;&c_o;&c_v;&c_o;&c_b;&c_r;&c_a;&c_n;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="rekrut.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="rekrut.0igi.MIL">
  <uzo tip="fak">MIL</uzo>
    <dif>
      Fari iun <tld/>o:
      <ekz>
        kial vi <tld/>igas lin, se li estas forta kaj sana
        <fnt>
          <bib>SatirRak</bib>
          <lok>Kuracistaj historioj</lok>
        </fnt>?
      </ekz>
      <ekz>
        la turkoj <tld/>igis &ccirc;iujn armenajn virojn, kapablajn uzi armilojn, kaj ilin
        sekrete bu&ccirc;is
        <fnt>
          <bib>Monato</bib>
          <aut>Susanna Gevorgjan</aut>
          <vrk><url ref="&Monato;/2006/008828.php">Krimo forgesita</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>reclutar</trd>,
      <trd>allistar</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>einberufen <klr>(zum Milit&auml;r)</klr></trd>,
      <trd>einziehen <klr>(zum Milit&auml;r)</klr></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>rekruteren</trd>,
      <trd>aanwerven</trd>,
      <trd>lichten</trd>
    </trdgrp>
  </snc>
  <snc mrk="rekrut.0igi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Varbi iun:
      <ekz>
        la dungantoj ne volis pagi vivtenantajn salajrojn al skotaj laboristoj, do ili
        <tld/>igis el Litovio &ccirc;efe agrikulturajn laboristojn sen spertoj
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2006/009062.php">Enmigrado anta kaj inta</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu kontrolas &ccirc;iujn &ccirc;i malpermesojn? &ccirc;u ni devas por tio <tld/>igi armeon da
        denuncantoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2007/009215.php">Malpermesoj</url></vrk>
          <lok>2007</lok>
        </fnt>?
      </ekz>
    </dif>
    <trd lng="ca">recollir</trd>
    <trd lng="de">anwerben</trd>
    <trd lng="nl">rekruteren</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_z;&c_y;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_u; &c_v;&c_o;&c_j;&c_s;&c_k;&c_a;)</klr></trd>,
    <trd>&c_r;&c_e;&c_k;&c_r;&c_u;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>odv&eacute;st na vojnu</trd>,
    <trd>povolat do arm&aacute;dy</trd>,
    <trd>verbovat</trd>
  </trdgrp>
  <trd lng="de">rekrutieren</trd>
  <trd lng="en">recruit</trd>
  <trd lng="fr">recruter</trd>
  <trd lng="ja">&#x5FB4;&#x52DF;&#x3059;&#x308B;  <pr>&#x3061;&#x3087;&#x3046;&#x307C;&#x3059;&#x308B;</pr></trd>
  <trdgrp lng="pl">
    <trd>rekrutowa&cacute;</trd>,
    <trd>powo&lstroke;ywa&cacute;</trd>
  </trdgrp>
  <trd lng="ro">recrutare</trd>
  <trdgrp lng="sk">
    <trd>odvies&tcaron; na vojnu</trd>,
    <trd>povola&tcaron; do arm&aacute;dy</trd>,
    <trd>verbova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_je;&c_k;&c_r;&c_u;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_z;&c_i;&c_v;&c_a;&c_t;&c_i; &c_v; &c_a;&c_r;&c_m;&c_ib;&c_ju;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_t;&c_i;/&c_v;&c_z;&c_ja;&c_t;&c_i; &c_d;&c_o; &c_v;&c_ib;&c_j;&c_s;&c_mol;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x62DB;&#x52DF; <pr>zh&#x101;om&#xF9;</pr></trd>,
    <trd>&#x62DB;&#x8058; <pr>zh&#x101;op&#xEC;n</pr></trd>,
    <trd>&#x5F81;&#x624D; <pr>zh&#x113;ngc&#xE1;i</pr></trd>,
    <trd>&#x5FB5;&#x624D; <pr>zh&#x113;ngc&#xE1;i</pr></trd>,
    <trd>&#x62DB;&#x4EBA; <pr>zh&#x101;or&#xE9;n</pr></trd>,
    <trd>&#x52DF; <pr>m&#xF9;</pr></trd>,
    <trd>&#x62DB;&#x8ACB; <pr>zh&#x101;oq&#x1D0;ng</pr></trd>,
    <trd>&#x62DB;&#x8BF7; <pr>zh&#x101;oq&#x1D0;ng</pr></trd>,
    <trd>&#x62DB;&#x5F55; <pr>zh&#x101;ol&#xF9;</pr></trd>,
    <trd>&#x62DB;&#x9304; <pr>zh&#x101;ol&#xF9;</pr></trd>
  </trdgrp>
</drv>

</art>
