<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>sveter</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sveter.0o">
  <kap><tld/>o</kap>
  <snc mrk="sveter.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Malformala torso<ref tip="super" cel="vest.0o.TEKS">vesto</ref> 
      el mola &scirc;tofo:
      <ekz>
        &scirc;i levas la brakojn, permesante al Niko demeti &scirc;ian <tld/>on
        <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 14a, p. 86a</lok></fnt>;
      </ekz>
      <ekz>
        anka&ubreve; mi vidas lian helan <tld/>on
        <fnt>
          <aut>P. Kuusberg, trad. A. Lekko</aut>,
          <vrk><url          
          ref="http://miresperanto.narod.ru/biblioteko/somermeze.htm">
          Somermeze</url>. - Eesti Raamat, 1981</vrk>,
          <lok>chapitro 4a, 2</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">sweater</trd>
  <trdgrp lng="nl">
    <trd>sweater</trd>,
    <trd>sweatshirt</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>su&eacute;ter</trd>,
    <trd>pul&ocirc;ver</trd>
  </trdgrp>
</drv>
</art>

