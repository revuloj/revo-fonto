<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sveter.xml,v 1.9 2023/11/23 21:23:54 revo Exp $">
<kap>
  <rad>sveter</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sveter.0o">
  <kap><tld/>o</kap>
  <snc mrk="sveter.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Malformala torso<ref tip="lst" lst="voko:vesta&jcirc;oj" cel="vest.0o.TEKS">vesto</ref>
      el mola &scirc;tofo:
      <ekz>
        &scirc;i levas la brakojn, permesante al Niko demeti &scirc;ian <tld/>on
        <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 14a, p. 86a</lok></fnt>;
      </ekz>
      <ekz>
        anka&ubreve; mi vidas lian helan <tld/>on
        <fnt>
          <aut>P. Kuusberg, trad. A. Lekko</aut>,
          <vrk><url ref="http://miresperanto.narod.ru/biblioteko/somermeze.htm">
          Somermeze</url>. - Eesti Raamat, 1981</vrk>,
          <lok>chapitro 4a, 2</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_v;&c_e;&c_d;&c_a;&c_r;</trd>,
    <trd>&c_s;&c_mol;&c_v;&c_ib;&c_t;&c_e;&c_r;</trd>
  </trdgrp>
  <trd lng="cs">svetr</trd>
  <trd lng="fr">sweater</trd>
  <trdgrp lng="nl">
    <trd>sweater</trd>,
    <trd>sweatshirt</trd>
  </trdgrp>
  <trd lng="pl">sweter</trd>
  <trdgrp lng="pt">
    <trd>su&eacute;ter</trd>,
    <trd>pul&ocirc;ver</trd>
  </trdgrp>
  <trd lng="sk">sveter</trd>
  <trd lng="uk">&c_s;&c_v;&c_je;&c_t;&c_r;</trd>
</drv>
</art>

