<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gondol.xml,v 1.27 2024/04/14 22:16:00 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>gondol</rad>/o
</kap>
<drv mrk="gondol.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Longa, plata <ref tip="super" cel="bark.0o">barketo</ref>,
      kun amba&ubreve; ekstrema&jcirc;oj levitaj
      en formo de cignokolo, movata per stangoj a&ubreve; remiloj, kaj
      uzata en <ref tip="malprt" cel="veneci.0o">Venecio</ref>:
      <ekz>
        la veneciaj <tld/>oj havas platan fundon, tiel ke ili povu
        sen&gcirc;ene transiri de la akvo al la tero kaj inverse
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=513229">Gondolo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        en la kanaloj inter la palac&scirc;ipoj veturis centoj de malgrandaj
        ar&gcirc;ent<tld/>oj &ccirc;iudirekten
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Ar&gcirc;enta Urbo Amarganto</lok>
        </fnt>;
      </ekz>
      <ekz>
        neniam en mia vivo mi <tld/>os tra la aero
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Vo&ccirc;o de la Kvieto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="gondol.0o.kajuto">
    <uzo tip="stl">EVI</uzo>
    <fnt>
      <bib>Viki</bib>
      <lok><url ref="https://eo.wikipedia.org/?curid=97211">Kosmolifto</url></lok>
    </fnt>
    <dif>
      <ref tip="malprt" cel="kajut.0o">Kajuto</ref> de aer&scirc;ipo:
      <ekz>
        la longbalono, en kiun <nom>Giffard</nom> konstruis la
        vaporma&scirc;inon,
        estis 44 m longa kaj havis la volumenon de 2&nbsp;500 kubaj metroj,
        <tld/>o kaj motoro pendi&gcirc;is sur trabo sub la balono
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=52959">Aer&scirc;ipo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_n;&c_d;&c_o;&c_l;&c_a;</trd>
  <trd lng="bg">&c_g;&c_o;&c_n;&c_d;&c_o;&c_l;&c_a;</trd>
  <trd lng="ca">g&oacute;ndola</trd>
  <trdgrp lng="cs">
    <trd>kabina vzducholodi</trd>,
    <trd>ko&scaron; bal&oacute;nu</trd>,
    <trd>prostor pro motor letadla</trd>
  </trdgrp>
  <trd lng="de">Gondel</trd>
  <trd lng="en">gondola</trd>
  <trd lng="es">g&oacute;ndola</trd>
  <trd lng="fr">gondole</trd>
  <trd lng="hu">gondola</trd>
  <trd lng="ja">&#x30B4;&#x30F3;&#x30C9;&#x30E9;  <pr>&#12372;&#12435;&#12393;&#12425;</pr></trd>
  <trd lng="nl">gondel</trd>
  <trd lng="pl">gondola</trd>
  <trd lng="pt">g&ocirc;ndola<klr tip="amb">
    (embarca&ccedil;&atilde;o)</klr></trd>
  <trd lng="ru">&c_g;&c_o;&c_n;&c_d;&c_o;&c_l;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>kab&iacute;na vzducholode</trd>,
    <trd>k&ocirc;&scaron; bal&oacute;nu</trd>
  </trdgrp>
  <trd lng="sv">gondol</trd>
  <trd lng="uk">&c_gu;&#x43E;&#x43D;&c_d;&#x43E;&#x43B;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x72ED;&#x957F;&#x5C0F;&#33337; <pr>xi&#xE1;ch&#xE1;ngxi&#x1CE;ochu&#xE1;n</pr></trd>,
    <trd>&#x4E24;&#22836;&#x5C16;&#30340;&#x5E73;&#x5E95;&#33337;
    <pr>li&#x1CE;ngt&#xF3;uji&amacron;ndep&#xED;ngd&#x1D0;chu&#xE1;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="gondol.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      <tld lit="G"/>veturigisto, <tld/>kondukanto:
      <ekz>
        la <tld/>isto kutime staras, sed lerte klini&gcirc;as anta&ubreve; pasotaj
        pontoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=162273">Venecio,
          trafiko</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="sxofor.0o">&scirc;oforo</ref>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_n;&c_d;&c_a;&c_l;&c_mol;&c_je;&c_r;</trd>
  <trd lng="bg">&c_g;&c_o;&c_n;&c_d;&c_o;&c_l;&c_i;&c_je;&c_r;</trd>
  <trd lng="ca">gondoler</trd>
  <trd lng="cs">gondoli&eacute;r</trd>
  <trdgrp lng="de">
     <trd>Gondoliere</trd>,
     <trd>Gondelf&uuml;hrer</trd></trdgrp>
  <trd lng="en">gondolier</trd>
  <trd lng="es">gondolero</trd>
  <trd lng="fr">gondolier</trd>
  <trd lng="hu">gondol&aacute;s</trd>
  <trd lng="ja">&#x30B4;&#x30F3;&#x30C9;&#x30E9;&#x306E;&#33337;&#x982D; 
  <pr>&#12372;&#12435;&#12393;&#12425;&#x306E;&#x305B;&#12435;&#12393;&#12358;</pr></trd>
  <trd lng="nl">gondelier</trd>
  <trd lng="pl">gondolier</trd>
  <trd lng="pt">remador de <ind>g&ocirc;ndola</ind></trd>
  <trd lng="ru">&c_g;&c_o;&c_n;&c_d;&c_o;&c_l;&c_mol;&c_je;&c_r;</trd>
  <trd lng="sk">gondolier</trd>
  <trd lng="sv">gondolj&auml;r</trd>
  <trd lng="uk">&c_g;&#x43E;&#x43D;&c_d;&#x43E;&#x43B;&#x44C;&c_jeu;&c_r;</trd>
</drv>
</art>
