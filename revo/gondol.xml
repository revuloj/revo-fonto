<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>gondol</rad>/o
</kap>
<drv mrk="gondol.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Longa, plata barketo, kun amba&ubreve; ekstrema&jcirc;oj levitaj en formo
      de cignokolo, movata per stangoj a&ubreve; remiloj, kaj uzata en Venecio. 
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_n;&c_d;&c_o;&c_l;&c_a;</trd>
  <trd lng="de">Gondel</trd>
  <trd lng="es">g&oacute;ndola</trd>
  <trd lng="fr">gondole</trd>
  <trd lng="hu">gondola</trd>
  <trd lng="nl">gondel</trd>
  <trd lng="pt">g&ocirc;ndola<klr tip="amb"> (embarca&ccedil;&atilde;o)</klr></trd>
  <trd lng="ru">&c_g;&c_o;&c_n;&c_d;&c_o;&c_l;&c_a;</trd>
  <trd lng="sv">gondol</trd>
</drv>

<drv mrk="gondol.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      <tld lit="G"/>veturigisto, <tld/>kondukanto.  
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_n;&c_d;&c_a;&c_l;&c_mol;&c_je;&c_r;</trd>
  <trdgrp lng="de">
     <trd>Gondoliere</trd>,
     <trd>Gondelf&uuml;hrer</trd></trdgrp>
  <trd lng="es">gondolero</trd>
  <trd lng="fr">gondolier</trd>
  <trd lng="hu">gondol&aacute;s</trd>
  <trd lng="nl">gondelier</trd>
  <trd lng="pt">remador de <ind>g&ocirc;ndola</ind></trd>
  <trd lng="ru">&c_g;&c_o;&c_n;&c_d;&c_o;&c_l;&c_mol;&c_je;&c_r;</trd>
</drv>
</art>
