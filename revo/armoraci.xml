<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: armoraci.xml,v 1.8 2022/12/25 08:51:06 revo Exp $">
<kap>
  <rad>armoraci</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="armoraci.0o">
  <kap><tld/>o</kap>
  <snc mrk="armoraci.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref>
      el familio de <ref tip="lst" lst="voko:brasikoparencoj" cel="brasik1.0oj">brasikacoj</ref>
      (<trd lng="la">Armoracia</trd>) kun la specio <ref tip="prt"
      cel="kren.0o.specio">kreno<sncref/></ref>:
      <ekz>
        kampara <tld/>o
        <fnt>
          <aut>Wouter F. Pilger </aut>
          <vrk><url ref="&STEB;vortaroj/legomoj/legomou8.htm">PPL
          LEgomoj &#8208; Esperanto-nomoj por legomoj man&gcirc;ataj en
          Nord-Okcidenta E&ubreve;ropo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        moringo <klr>[&#8230;]</klr> entenas en la sukulentaj partoj
        sinapolean glikozidon,
        kiu odoras forte kiel <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=289951">Moringo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/b/b6/Armoracia_rusticana.jpg/360px-Armoracia_rusticana.jpg"
    prm="CC-BY-SA-3.0">Kampara <tld/>o (Armoracia rusticana)
        <fnt>
          <aut>Pethan</aut>
          <lok><url
          ref="&WCW;/File:Armoracia_rusticana.jpg">&WCW;/File:Armoracia_rusticana.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Armoracia</trd>
     <trd lng="pt">raiz-forte</trd>
  </snc>
  <trd lng="be">&c_h;&c_r;&c_e;&c_n;</trd>
  <trd lng="fr">armoracia</trd>
  <trd lng="he">&shin;&#1473;&#1460;&resh;&yod;&vav;&#1465;&fnun;</trd>
  <trd lng="uk">&c_h;&c_r;&c_ib;&c_n;</trd>
</drv>
</art>

