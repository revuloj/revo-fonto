<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kadi.xml,v 1.20 2022/03/21 17:51:56 revo Exp $">
<kap>
<rad>kadi</rad>/o
</kap>
<drv mrk="kadi.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">ISL</uzo>
  <snc>
    <dif>
      Islama &scirc;tatoficisto, komisiita precipe ju&gcirc;adi 
      la&ubreve; la
      <ref tip="vid" cel="koran.0o">Korano</ref> kaj
      <ref tip="vid" cel="suna.0o">Sunao</ref>, a&ubreve; la&ubreve; la 
      konsultoj de
      <ref tip="vid" cel="mufti.0o">muftio</ref>:
      <ekz>
        lia familio okupi&gcirc;is ekde generacioj pri religia juro nome &scirc;ario, tiel anka&ubreve; li
        i&gcirc;is jursciencisto, <tld/>o, li decidis en a&gcirc;o de 21 jaroj plenumi la
        pilgrimon al Mekko, deviga por &ccirc;iu islamano
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19211">Ibn Batuta</url></lok>
        </fnt>.
     </ekz>
    </dif>
    <ref tip="super" cel="jugx.0isto">ju&gcirc;isto</ref>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_d;&c_z;&c_ib;</trd>
  <trd lng="bg">&c_k;&c_a;&c_d;&c_i;&c_ja;</trd>
  <trd lng="ca">cadi</trd>
  <trd lng="cs">k&aacute;d&iacute;</trd>
  <trd lng="de">Kadi</trd>
  <trdgrp lng="en">
     <trd>cadi</trd>,
     <trd>kadee</trd>
   </trdgrp>
  <trd lng="es">cad&iacute;</trd>
  <trd lng="fa">&a_q;&a_A;&a_D;&f_y; &a_sh;&a_r;&a_ayn;</trd>
  <trd lng="fi">kadi</trd>
  <trd lng="fr">cadi</trd>
  <trd lng="he">&qof;&alef;&dalet;&yod;</trd>
  <trd lng="hu">k&aacute;di</trd>
  <trd lng="id">kadi</trd>
  <trd lng="nl">kadi</trd>
  <trd lng="pl">kadi</trd>
  <trd lng="pt">c&aacute;di</trd>
  <trd lng="ru">&c_k;&c_a;&c_d;&c_i;</trd>
  <trdgrp lng="sk">
    <trd>k&aacute;d&iacute;</trd>,
    <trd>sudca v&nbsp;islamsk&yacute;ch krajin&aacute;ch</trd>
  </trdgrp>
  <trd lng="sv">qadi</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x430;&#x434;&#x456;</trd>,
    <trd>&#x43A;&#x430;&#x434;&#x456;&#x439;</trd>
  </trdgrp>
</drv>
</art>

