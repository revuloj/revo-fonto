<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>kadi</rad>/o
</kap>
<drv mrk="kadi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ISL</uzo>
  <snc>
    <dif>
      Islama &scirc;tatoficisto, komisiita precipe ju&gcirc;adi la&ubreve; la
      <ref tip="vid" cel="koran.0o">Korano</ref> kaj
      <ref tip="vid" cel="suna.0o">Sunao</ref>, a&ubreve; la&ubreve; la konsultoj de
      <ref tip="vid" cel="mufti.0o">muftio</ref>.
    </dif>
  </snc>
  <trd lng="ca">cadi</trd>
  <trd lng="de">Kadi</trd>
  <trdgrp lng="en">
    <trd>cadi</trd>,
    <trd>kadee</trd>
  </trdgrp>
  <trd lng="fr">cadi</trd>
  <trd lng="he">&qof;&alef;&dalet;&yod;</trd>
  <trd lng="hu">k&aacute;di</trd>
  <trd lng="ru">&c_k;&c_a;&c_d;&c_i;</trd>
</drv>
</art>

