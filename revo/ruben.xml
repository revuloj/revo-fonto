<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ruben.xml,v 1.24 2024/04/14 06:28:51 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ruben</rad>/o
</kap>
<drv mrk="ruben.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="ruben.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Diafana <ref tip="super" cel="sxton.juvel0o.MIN">juvel&scirc;tono</ref>,
      speco de <ref tip="super" cel="korund.0o">korundo</ref>,
      pro aldono de kromoksido ordinare malhele ru&gcirc;a:
      <ekz>
        la &scirc;tonoj de vico: <tld/>o, topazo, kaj smeraldo
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 28:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i estis ornamita per diamantoj kaj <tld/>oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        garnita per diamantoj, <tld/>oj kaj safiroj
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiun &ccirc;i <tld/>on mi detiris de la fingro de unu ministro kiun mi dum &ccirc;asado
        mortigis
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        kokete pendolis sub &scirc;ia liva orelo <tld/>o
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 63a, volumo 2a, p.
        359a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="safir.0o.MIN">safiro</ref>,
      <ref cel="smirg.0o.MIN">smirgo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_u;&c_b;&c_ib;&c_n;</trd>
  <trd lng="bg">&c_r;&c_u;&c_b;&c_i;&c_n;</trd>
  <trd lng="ca">rob&iacute;</trd>
  <trd lng="cs">rub&iacute;n</trd>
  <trd lng="de">Rubin</trd>
  <trd lng="en">ruby</trd>
  <trd lng="es">rub&iacute;</trd>
  <trd lng="fr">rubis</trd>
  <trd lng="hu">rubin</trd>
  <trdgrp lng="ja">
    <trd>&#x30EB;&#x30D3;&#x30FC;</trd>,
    <trd>&#x7D05;&#x7389;  <pr>&#x3053;&#x3046;&#x304E;&#x3087;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">robijn</trd>
  <trd lng="pl">rubin</trd>
  <trdgrp lng="pt">
    <trd>rubi</trd>,
    <trd>rubim</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_u;&c_b;&c_i;&c_n;</trd>
  <trd lng="sk">rub&iacute;n</trd>
  <trd lng="uk">&#x440;&#x443;&#x431;&#x456;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x7D05;&#x5BF6;&#x77F3; <pr>h&#xF3;ngb&#x1CE;osh&#xED;</pr></trd>,
    <trd>&#x7EA2;&#x5B9D;&#x77F3; <pr>h&#xF3;ngb&#x1CE;osh&#xED;</pr></trd>
  </trdgrp>
</drv>
</art>
