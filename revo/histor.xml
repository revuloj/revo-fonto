<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc> <rad>histori</rad>/o
</kap>
<drv mrk="histor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="histor.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="super" cel="scienc.0o.fako">Scienco</ref>
      studanta kronologie la pasintajn okaza&jcirc;ojn de la homaro
      kaj la sociajn faktojn:
      <ekz>
        geografio kaj kronologio estas la du okuloj de la <tld/>o;
      </ekz>
      <ekz>
        <tld/>a verko, senpartieco;
      </ekz>
      <ekz>
        <ind><tld/>isto</ind>.        
        <trd lng="be">&c_g;&c_ib;&c_s;&c_t;&c_o;&c_r;&c_y;&c_k;</trd>
        <trd lng="br">istorour</trd>
        <trd lng="es">historiador</trd>
        <trd lng="fr">historien</trd>
        <trd lng="he">&he;&yod;&samekh;&tet;&vav;&resh;&yod;&vav;&fnun;</trd>
        <trd lng="hu">t&ouml;rt&eacute;n&eacute;sz</trd>
      </ekz>
    </dif>
    <trd lng="hu">t&ouml;rt&eacute;nettudom&aacute;ny</trd>
  </snc>
  <snc mrk="histor.0o.kroniko">
    <dif>
      Rakonto pri gravaj memorindaj okaza&jcirc;oj,
      kronologie aran&gcirc;itaj:
      <ekz>
        antikva <tld/>o;
      </ekz>
      <ekz>
        la <tld/>o de la Centjara milito, de Jesuo;
      </ekz>
      <ekz>
        la <tld/>o de mia vivo.
      </ekz>
    </dif>
    <ref tip="vid" cel="kronik1.0o">kroniko</ref>
    <trd lng="hu">t&ouml;rt&eacute;net</trd>
  </snc>
  <snc mrk="histor.0o.rakonto">
    <dif>
      Rakonto pri aparta kaj iom ampleksa okaza&jcirc;o, kiu
      trafis iun:
      <ekz>
        la <tld/>o de la korvo kun vulpo;
      </ekz>
      <ekz>
        la eksterordinaraj <tld/>oj de E. Poe;
      </ekz>
      <ekz>
        en la tuta mondo neniu scias tiom multe da <tld/>oj,
        kiel la dio de dormo
        <fnt>Z</fnt>;
      </ekz>
      <ekz>
        belaj <tld/>oj por infanoj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fabel.0o">fabelo</ref>,
      <ref cel="roman.0o">romano</ref>,
      <ref cel="novel.0o">novelo</ref>.
    </refgrp>
    <trd lng="he">&samekh;&yod;&pe;&vav;&resh;</trd>
    <trd lng="hu">t&ouml;rt&eacute;net</trd>
  </snc>
  <snc>
    <dif>
      Tuto de la sinsekvaj statoj de esta&jcirc;o, kiu
      &scirc;an&gcirc;i&gcirc;as.
    </dif>
    <refgrp tip="vid">
      <ref cel="evolu.0o">evoluo</ref>,
      <ref cel="proces.0o">proceso 2</ref>.
    </refgrp>
    <trd lng="hu">t&ouml;rt&eacute;nelem</trd>
  </snc>                        
  <trd lng="be">&c_g;&c_ib;&c_s;&c_t;&c_o;&c_r;&c_y;&c_ja;</trd>
  <trd lng="br">istor</trd>
  <trd lng="de">Geschichte</trd>
  <trd lng="en">history</trd>
  <trd lng="es">historia</trd>
  <trd lng="fr">histoire</trd>
  <trd lng="he">&he;&yod;&samekh;&tet;&vav;&resh;&yod;&he;</trd>
  <trdgrp lng="nl">
    <trd>geschiedenis</trd>,
    <trd>historie</trd>
  </trdgrp>
  <trd lng="pt">hist&oacute;ria</trd>
  <trd lng="ru">&c_i;&c_s;&c_t;&c_o;&c_r;&c_i;&c_ja;</trd>
  <trd lng="sv">historia</trd>
</drv>

<drv mrk="histor.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta <tld/>on:
      <ekz>
        <tld/>a verko.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Registrita de la <tld/>istoj, efektiva:
      <ekz>
        <tld/>a fakto..
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Pri kiu oni posedas skribitajn dokumentojn:
      <ekz>
        <tld/>aj tempoj.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Registrinda de la <tld/>o, tre grava, decida:
      <ekz>
        <tld/>a momento.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_s;&c_t;&c_a;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd> 
  <trd lng="br">istorel</trd>
  <trd lng="cs">historick&yacute;</trd>
  <trd lng="de">historisch</trd>
  <trd lng="es">hist&oacute;rico</trd>
  <trd lng="fr">historique</trd>
  <trd lng="he">&he;&yod;&samekh;&tet;&vav;&resh;&yod;</trd>
  <trd lng="hu">t&ouml;rt&eacute;nelmi</trd>
  <trdgrp lng="nl">
    <trd>geschiedkundig</trd>,
    <trd>historisch</trd>
  </trdgrp>
  <trd lng="pt">hist&oacute;rico</trd>
  <trd lng="ru">&c_i;&c_s;&c_t;&c_o;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sv">historisk</trd>
</drv>

<drv mrk="histor.antaux0o">
  <kap>anta&ubreve;<tld/>o</kap>
  <snc>
    <ref tip="dif" cel="histor.pra0o">pra<tld/>o.</ref>
  </snc>
</drv>

<drv mrk="histor.pra0o">
  <kap>pra<tld/>o</kap>
  <uzo tip="fak">PRA</uzo>
  <snc>
    <dif>
      Parto de la arkeologio, kiu studas la pratempon, pri kiu ni havas
      neniujn skribitajn dokumentojn.
    </dif>
    <refgrp tip="sin">
      <ref cel="histor.antaux0o">anta&ubreve;<tld/>o</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_g;&c_ib;&c_s;&c_t;&c_o;&c_r;&c_y;&c_ja;</trd>    
  <trd lng="br">ragistor</trd>
  <trd lng="de">Urgeschichte</trd>
  <trd lng="es">prehistoria</trd>
  <trd lng="fr">pr&eacute;histoire</trd>
  <trd lng="he">&pe;&resh;&he;&yod;&samekh;&tet;&vav;&resh;&yod;&he;</trd>
  <trd lng="hu">&odblac;st&ouml;rt&eacute;net</trd>
  <trd lng="nl">prehistorie</trd>
  <trd lng="pt">pr&eacute;-hist&oacute;ria</trd>
</drv>
</art>
