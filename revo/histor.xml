<drv mrk="histor.0a">
  <kap><tld/>a</kap>
  <snc mrk="histor.0a.HIS">
    <dif>
      De la <tld/>o, registrita de <tld/>istoj, pri kiu oni havas
      skribitajn dokumentojn, efektiva:
      <ekz>
        <tld/>aj tempoj;
      </ekz>
      <ekz>
        <tld/>a fakto
        <fnt><bib>Lanti</bib>, <lok>de la deziro al la
        efektivigo</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        momento <tld/>a
        <klr>(<ref tip="vid" cel="epok.0fara.KOMUNE">epokfara</ref>)</klr>;
      </ekz>
      <ekz>
        <tld/>aj kondi&ccirc;oj
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi legas <klr>[&#x2026;]</klr> <tld/>a&jcirc;on
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ja">
      <trd>&#x6B74;&#x53F2;&#x4E0A;&#x306E; [<ind>&#x308C;&#x304D;&#x3057;&#x3058;&#x3087;&#x3046;&#x306E;</ind>]</trd>,
      <trd>&#x6B74;&#x53F2;&#x7684;&#x306A; [<ind>&#x308C;&#x304D;&#x3057;&#x3066;&#x304D;&#x306A;</ind>]</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>historyczny</trd>,
      <trd>dziejowy</trd>
    </trdgrp>
  </snc>
  <snc mrk="histor.0a.SCI">
    <dif>
      Rilatanta la <tld/>an, la <tld/>an sciencon:
      <ekz>
        malnova <tld/>a libro
        <fnt>
          <bib>Fab3</bib>
          <lok>Juda knabino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la muroj estis kovritaj per <klr>[&#x2026;]</klr> <tld/>aj skizoj kaj pejza&gcirc;oj
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ja">
      <trd>&#x6B74;&#x53F2;&#x306E; [<ind>&#x308C;&#x304D;&#x3057;&#x306E;</ind>]</trd>,
      <trd>&#x6B74;&#x53F2;&#x306B;&#x95A2;&#x3059;&#x308B; [<ind>&#x308C;&#x304D;&#x3057;&#x306B;&#x304B;&#x3093;&#x3059;&#x308B;</ind>]</trd>,
      <trd>&#x6B74;&#x53F2;&#x5B66;&#x306E; [<ind>&#x308C;&#x304D;&#x3057;&#x304C;&#x304F;&#x306E;</ind>]</trd>
    </trdgrp>
    <trd lng="pl">historyczny</trd>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_s;&c_t;&c_a;&c_r;&c_y;&c_ch;&c_n;&c_y;
  </trd>
  <trd lng="br">istorel</trd>
  <trdgrp lng="cs">
    <trd>d&ecaron;jinn&yacute;</trd>,
    <trd>historick&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>historisch</trd>,
    <trd>geschichtlich</trd>
  </trdgrp>
  <trd lng="en">historical</trd>
  <trd lng="es">hist&oacute;rico</trd>
  <trd lng="fr">historique</trd>
  <trd lng="he">&he;&yod;&samekh;&tet;&vav;&resh;&yod;</trd>
  <trd lng="hu">t&ouml;rt&eacute;nelmi</trd>
