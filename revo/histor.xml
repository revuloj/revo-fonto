  <uzo tip="stl">ARK</uzo>
  <snc mrk="histor.natur0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      <tld lit="H"/>a grupiga nomo de sciencaj fakoj studantaj la naturon, aparte viva&jcirc;ojn, sed 
      anka&ubreve; mineralojn, astrojn k.a.:
      <ekz>
        apena&ubreve;aj varia&jcirc;oj <klr>[&#x2026;]</klr> estas malofte konsiderataj
        registrindaj en verkoj de natur<tld/>o
        <fnt>
          <bib>OriginoDeSpecioj</bib>,
          <lok>&ccirc;apitro 2a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>natur<tld/>a muzeo</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Izabel C. O. Santiago</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006900.html">Fa&ubreve;no: La &leftquot;drakoj&rightquot; de
          Komodo: Gigantaj bestoj sur izola insulo</url></vrk>
        </fnt>.
        <trdgrp lng="de">
          <trd>naturhistorisches Museum</trd>,
          <trd>Naturkundemuseum</trd>
        </trdgrp>
        <trdgrp lng="en">
          <trd>natural history museum</trd>,
          <trd>museum of natural history</trd>
        </trdgrp>
        <trdgrp lng="es">
