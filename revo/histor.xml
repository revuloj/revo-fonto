  </trdgrp>
</drv>

<drv mrk="histor.viv0o">
  <kap>viv<tld/>o</kap>
  <snc mrk="histor.viv0o.KOMUNE">
    <dif>
      <tld lit="H"/>o de ies vivo kaj traviva&jcirc;oj;
      <ref tip="sin" cel="biogra1.0o">biografio</ref>:
      <ekz>
        ni mallonge sciigis unu al la alia niajn viv<tld/>ojn post nia
        disi&gcirc;o
        <fnt><aut>I. G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>,
        <lok>[1898?]</lok></fnt>;
      </ekz>
      <ekz>
        rekonstruo de ies viv<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="https://www.monato.be/2004/008063.html">50-jara stabileco en verkado - &ccirc;u
          klasika?</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia leginda viv<tld/>o, origine franclingve skribita la&ubreve; instigo de liaj du
        filinoj, nun aperis en kompetenta Esperanto-traduko
        <fnt>
          <bib>Monato</bib>
          <aut>Andreas K&uuml;nzli</aut>
          <vrk><url ref="https://www.monato.be/2013/011020.php">Atentinda memuaro</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Lebenslauf</trd>,
    <trd>Lebensgeschichte</trd>,
    <trd>Biographie</trd>
