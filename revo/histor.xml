          <trd>museo de ciencias naturales</trd>
        </trdgrp>
        <trd lng="fr">mus&eacute;e d&#x2019;histoire naturelle</trd>
        <trd lng="ja">&#x81EA;&#x7136;&#x53F2;&#x535A;&#x7269;&#x9928; [<ind>&#x3057;&#x305C;&#x3093;&#x3057;&#x306F;&#x304F;&#x3076;&#x3064;&#x304B;&#x3093;</ind>]</trd>
        <trd lng="nl">natuurhistorisch museum</trd>
        <trd lng="pt">museu de hist&oacute;ria natural</trd>
        <trd lng="sv">naturhistoriskt museum</trd>
