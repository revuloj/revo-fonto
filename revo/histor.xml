  <trd lng="pt">hist&oacute;ria natural</trd>
</drv>

<drv mrk="histor.pra0o">
  <kap>pra<tld/>o, <var><kap>anta&ubreve;<tld/>o</kap></var></kap>
  <snc mrk="histor.pra0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">PRA</uzo>
    <dif>
      Parto de la <ref tip="malprt" cel="histor.0o.okazajxoj">historio</ref>, kiu ne postlasis
      skribitajn dokumentojn:
      <ekz>
        grup-spiriteco, en kulto, estas la rezulto de miljaroj da pra<tld/>o, kiam homoj
        lo&gcirc;is en tribaj komunumoj
        <fnt>
          <bib>Monato</bib>
          <aut>W. H. Simcock</aut>
          <vrk><url ref="https://www.monato.be/2007/009197.php">Kio estas spiriteco?</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        antropologoj taksas je 10&#x202F;% la nombron de murdoj en la pra<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="https://www.monato.be/2012/010793.php">Neniam tiel pacemaj</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ja">&#x5148;&#x53F2;&#x6642;&#x4EE3; [<ind>&#x305B;&#x3093;&#x3057;&#x3058;&#x3060;&#x3044;</ind>]</trd>
    <trdgrp lng="pl">
      <trd>prahistoria</trd>,
      <trd>pradzieje</trd>
    </trdgrp>
  </snc>
  <snc mrk="histor.pra0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">PRA</uzo>
    <dif>
      <ref tip="super" cel="scienc.0o.fako">Scienco</ref>, kiu studas
      partojn de <tld/>o sen skribitaj atestoj, precipe helpe de <ref tip="vid" cel="arkeol1.0o">arkeologio</ref>.
    </dif>
    <trd lng="ja">&#x5148;&#x53F2;&#x5B66; [<ind>&#x305B;&#x3093;&#x3057;&#x304C;&#x304F;</ind>]</trd>
    <trd lng="pl">prahistoria</trd>
  </snc>
  <trd lng="be">
  &c_p;&c_r;&c_a;&c_g;&c_ib;&c_s;&c_t;&c_o;&c_r;&c_y;&c_ja;</trd>
  <trd lng="br">ragistor</trd>
  <trdgrp lng="cs">
    <trd>prahistorie</trd>,
    <trd>prap&uring;vod</trd>,
    <trd>prav&ecaron;k</trd>,
    <trd>p&rcaron;edhistorie</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Vorgeschichte</trd>,
    <trd>Urgeschichte</trd>
  </trdgrp>
  <trd lng="en">prehistory</trd>
  <trd lng="es">prehistoria</trd>
  <trd lng="fr">pr&eacute;histoire</trd>
  <trd lng="he">&pe;&resh;&he;&yod;&samekh;&tet;&vav;&resh;&yod;&he;</trd>
  <trd lng="hu">&odblac;st&ouml;rt&eacute;net</trd>
  <trd lng="id">prasejarah</trd>
  <trd lng="io">prehistorio</trd>
  <trd lng="it">preistoria</trd>
  <trd lng="nl">prehistorie</trd>
  <trd lng="pt">pr&eacute;-hist&oacute;ria</trd>
