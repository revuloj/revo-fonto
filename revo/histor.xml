<drv mrk="histor.0a">
  <kap><tld/>a</kap>
  <snc mrk="histor.0a.HIS">
    <dif>
      De la <tld/>o, registrita de <tld/>istoj, pri kiu oni havas
      skribitajn dokumentojn, efektiva:
      <ekz>
        <tld/>aj tempoj;
      </ekz>
      <ekz>
        <tld/>a fakto
        <fnt><bib>Lanti</bib> <lok>De la deziro al la
        efektivigo</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        momento <tld/>a
        <klr>(<ref tip="vid" cel="epok.0fara.KOMUNE">epokfara</ref>)</klr>;
      </ekz>
      <ekz>
        <tld/>aj kondi&ccirc;oj
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi legas <klr>[&#8230;]</klr> <tld/>a&jcirc;on
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ja">
      <trd>&#27508;&#21490;&#19978;&#12398; <pr>&#12428;&#12365;&#12375;&#12376;&#12423;&#12358;&#12398;</pr></trd>,
      <trd>&#27508;&#21490;&#30340;&#12394; <pr>&#12428;&#12365;&#12375;&#12390;&#12365;&#12394;</pr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>historyczny</trd>,
      <trd>dziejowy</trd>
    </trdgrp>
  </snc>
  <snc mrk="histor.0a.SCI">
    <dif>
      Rilatanta la <tld/>an, la <tld/>an sciencon:
      <ekz>
        malnova <tld/>a libro
        <fnt>
          <bib>Fab3</bib>
          <lok>Juda knabino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la muroj estis kovritaj per <klr>[&#8230;]</klr> <tld/>aj skizoj kaj pejza&gcirc;oj
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ja">
      <trd>&#27508;&#21490;&#12398; <pr>&#12428;&#12365;&#12375;&#12398;</pr></trd>,
      <trd>&#27508;&#21490;&#12395;&#38306;&#12377;&#12427;
      <pr>&#12428;&#12365;&#12375;&#12395;&#12363;&#12435;&#12377;&#12427;</pr></trd>,
      <trd>&#27508;&#21490;&#23398;&#12398;
      <pr>&#12428;&#12365;&#12375;&#12364;&#12367;&#12398;</pr></trd>
    </trdgrp>
    <trd lng="pl">historyczny</trd>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_s;&c_t;&c_a;&c_r;&c_y;&c_ch;&c_n;&c_y;
  </trd>
  <trd lng="br">istorel</trd>
