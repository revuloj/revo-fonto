<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: krut.xml,v 1.20 2020/11/08 10:34:00 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>krut</rad>/a
</kap>
<drv mrk="krut.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="krut.0a.GEOG">
    <dif>
      Havanta deklivon preska&ubreve; vertikalan; tre malfacile supren irebla:
      <ekz>
        <tld/>a monto, insulo, bordo;
      </ekz>
      <ekz>
        sur <tld/>a &scirc;tuparo li levis sin al la tegmento de la domo
        <fnt>
          <bib>F</bib>
          <lok>&FE; 34</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj tegmentoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la remparo <tld/>e mallevi&gcirc;as
        <fnt>
          <bib>Fab2</bib>
          <lok>Bildo el kastelremparo</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen ili staris sur <tld/>a deklivo apud klara lago
        <fnt>
          <bib>Fab3</bib>
          <lok>Ib kaj malgranda kristino</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur&ccirc;evale oni devas levi&gcirc;i la&ubreve; <tld/>aj montaj vojetoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[je]</klr> la flanko interna la ringa montaro estis tute <tld/>a kaj formis
        grandegan rondvalon
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="abrupt.0a">abrupta</ref>,
      <ref cel="dekliv">dekliva</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_r;&c_o;&c_m;&c_y;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_o;&c_m;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>p&rcaron;&iacute;kr&yacute;</trd>,
    <trd>prudk&yacute;</trd>,
    <trd>p&rcaron;&iacute;kr&yacute;</trd>,
    <trd>sr&aacute;zn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>steil</trd>,
    <trd>schroff</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>steep</trd>,
    <trd>abrupt</trd>,
    <trd>sheer</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>abrupto</trd>,
    <trd>escarpado</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>abrupte<klr> (pente)</klr></trd>,
    <trd>raide<klr> (pente)</klr></trd>
  </trdgrp>
  <trd lng="hu">meredek</trd>
  <trd lng="nl">steil</trd>
  <trdgrp lng="pl">
    <trd>stromy</trd>,
    <trd>pochy&lstroke;y</trd>,
    <trd>spadzisty</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>&iacute;ngreme</trd>,
    <trd>escarpado</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>abrupt</trd>,
    <trd>cursiv</trd>,
    <trd>pov&acirc;rnit</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_u;&c_t;&c_o;&c_j;</trd>,
    <trd>&c_o;&c_t;&c_v;&c_je;&c_s;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pr&iacute;kry</trd>,
    <trd>strm&yacute;</trd>
  </trdgrp>
  <trd lng="sv">brant</trd>
</drv>

<drv mrk="krut.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="krut.0i.GEOG">
    <dif>
      Stari <tld/>e:
      <ekz>
        la flanko de la roko rekte kaj subite <tld/>is malsupren;
      </ekz>
      <ekz>
        &ccirc;ie akraj, pintaj &scirc;tonegoj <tld/>is.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">&ccaron;n&iacute;t</trd>
  <trdgrp lng="de">
    <trd>steil sein</trd>,
    <trd>steil aufragen</trd>,
    <trd>steil abfallen</trd>,
    <trd>schroff aufragen</trd>,
    <trd>schroff abfallen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>b&eacute;er<klr> (ravin etc.)</klr></trd>,
    <trd>s'&eacute;lever<klr> (escarpement)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>meredek</trd>,
    <trd>meredeken &aacute;ll</trd>,
    <trd>meredeken esik</trd>
  </trdgrp>
  <trd lng="nl"><ind>steil</ind> zijn</trd>
  <trdgrp lng="pl">
    <trd>wznie&sacute;&cacute; si&eogonek;</trd>,
    <trd>wznosi&cacute; si&eogonek; stromo</trd>,
    <trd>stromym by&cacute;</trd>
  </trdgrp>   
<trdgrp lng="ro">
    <trd>av&acirc;nta</trd>,
    <trd>cre&#537;te vertiginos</trd>,
    <trd>fi abrupte</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>strmie&tcaron;</trd>,
    <trd>strmo st&aacute;&tcaron;</trd>,
    <trd>vyp&iacute;na&tcaron; sa</trd>,
    <trd>&ccaron;nie&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="krut.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="krut.0ajxo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <tld/>a parto de loko, ekzemple de marbordo:
      <ekz>
        akvo, falanta de <tld/>a&jcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Mi&hcirc;a 1:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen kuris la tuta grego malsupren de la <tld/>a&jcirc;o en la maron
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 8:32</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <klr>[&#8230;]</klr> kondukis lin &gcirc;is la <tld/>a&jcirc;o de la monteto, <klr>[&#8230;]</klr>
        por &jcirc;eti lin malsupren
        <fnt>
          <bib>NT</bib>
          <lok>Luko 4:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen &scirc;tono falas de <tld/>a&jcirc;o
        <fnt>
          <aut>Vladimir Glazunov</aut>,
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Noveloj/Cxarmo.html">La
          &ccirc;armo de &Ccirc;ielmontaro</url></vrk>,
          <lok>Fonto, numero 37a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="klif.0o">klifo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_r;&c_o;&c_m;&c_a;</trd>,
    <trd>&c_a;&c_d;&c_h;&c_o;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>steile Stelle</trd>,
    <trd>Steilk&uuml;ste</trd>,
    <trd>steil aufragende K&uuml;ste</trd>,
    <trd>steil aufragender Fels</trd>,
    <trd>steil abfallende K&uuml;ste</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bluff</trd>,
    <trd>cliff</trd>,
    <trd>escarpment</trd>,
    <trd>steep</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>escarpement</trd>,
    <trd>falaise</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd lng="hu">meredek part</trd>,
    <trd>szakad&eacute;k</trd>,
    <trd>mered&eacute;ly</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd><ind>steil</ind>e helling</trd>,
    <trd><ind>steil</ind>e rots</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>stromizna</trd>,
    <trd>urwisko</trd>,
    <trd>zr&aogonek;b</trd>
  </trdgrp>   
<trdgrp lng="ro">
    <trd>pr&abreve;v&abreve;li&#537;</trd>,
    <trd>st&acirc;nc&abreve;</trd>,
    <trd>baz&abreve;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_u;&c_ch;&c_a;</trd>,
    <trd>&c_o;&c_t;&c_k;&c_o;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>risco</trd>,
    <trd>farall&oacute;n</trd>,
    <trd>barranco</trd>,
    <trd>despe&ntilde;adero</trd>,
    <trd>precipicio</trd>
  </trdgrp>
</drv>

<drv mrk="krut.inter0ejo">
  <kap>inter<tld/>ejo</kap>
  <snc mrk="krut.inter0ejo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Tre mallar&gcirc;a irejo inter du <tld/>a&jcirc;oj de monto:
      <ekz>
        mi renkontis &scirc;in en <klr>[&#8230;]</klr> la inter<tld/>ejo
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Termopiloj estas inter<tld/>ejo, mallar&gcirc;a trapasejo &ccirc;e la landlimo
        <fnt>
          <vrk><url ref="https://neciklopedio.org/wiki/Termopiloj"
              >Termopiloj - Neciklopedio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="ravin.0o">ravino</ref>
  </snc>
  <trd lng="be">&c_c;&c_ja;&c_s;&c_mol;&c_n;&c_ib;&c_n;&c_a;</trd>
  <trd lng="cs">sout&ecaron;ska</trd>
  <trdgrp lng="de">
    <trd>Klamm</trd>,
    <trd>Schlucht</trd>
  </trdgrp>
  <trd lng="en">defile</trd>
  <trdgrp lng="es">
    <trd>desfiladero</trd>,
    <trd>ca&ntilde;&oacute;n</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;fil&eacute;<klr> (de montagne)</klr></trd>
  <trdgrp lng="hu">
    <trd>hasad&eacute;k</trd>,
    <trd>szurdok</trd>
  </trdgrp>
  <trd lng="nl">canyon</trd>
  <trd lng="pl">stromy w&aogonek;w&oacute;z</trd>     
  <trd lng="ro">cheile abrupte</trd>
  <trd lng="ru">&c_u;&c_shch;&c_je;&c_l;&c_mol;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>ties&ncaron;ava <klr>(horsk&aacute;)</klr></trd>,
    <trd>&uacute;&zcaron;ina</trd>
  </trdgrp>
</drv>
</art>
