<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>krut</rad>/a
</kap>
<drv mrk="krut.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="krut.0a.GEOG">
    <dif>
      Havanta deklivon preska&ubreve; vertikalan; tre malfacile suprenirebla:
      <ekz>
        <tld/>a monto, insulo, bordo;
      </ekz>
      <ekz>
        <tld/>e dekliva vojeto;
      </ekz>
      <ekz>
        <tld/>valo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="abrupt.0a">abrupta</ref>,
      <ref cel="dekliv">dekliva</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_r;&c_o;&c_m;&c_y;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_o;&c_m;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>p&rcaron;&iacute;kr&yacute;</trd>,
    <trd>prudk&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>steil</trd>,
    <trd>schroff</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>steep</trd>,
    <trd>abrupt</trd>,
    <trd>sheer</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>abrupte<klr> (pente)</klr></trd>,
    <trd>raide<klr> (pente)</klr></trd>
  </trdgrp>
  <trd lng="hu">meredek</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_u;&c_t;&c_o;&c_j;</trd>,
    <trd>&c_o;&c_t;&c_v;&c_je;&c_s;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">brant</trd>
</drv>

<drv mrk="krut.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="krut.0i.GEOG">
    <dif>
      Stari <tld/>e:
      <ekz>
        la flanko de la roko rekte kaj subite <tld/>is malsupren;
      </ekz>
      <ekz>
        &ccirc;ie akraj, pintaj &scirc;tonegoj <tld/>is.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>steil sein</trd>,
    <trd>steil aufragen</trd>,
    <trd>steil abfallen</trd>,
    <trd>schroff aufragen</trd>,
    <trd>schroff abfallen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>b&eacute;er<klr> (ravin etc.)</klr></trd>,
    <trd>s'&eacute;lever<klr> (escarpement)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>meredek</trd>,
    <trd>meredeken &aacute;ll</trd>,
    <trd>meredeken esik</trd>
  </trdgrp>
</drv>

<drv mrk="krut.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="krut.0ajxo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <tld/>a parto de loko, ekzemple de marbordo:
      <ekz>
        jen &scirc;tono falas de <tld/>a&jcirc;o
        <fnt>
        <aut>Vladimir Glazunov</aut>,
        <vrk><url
        ref="http://donh.best.vwh.net/Esperanto/Literaturo/Noveloj/Cxarmo.html">La
        &ccirc;armo de &Ccirc;ielmontaro</url></vrk>,
        <lok>Fonto, numero 37a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="klif.0o">klifo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_r;&c_o;&c_m;&c_a;</trd>,
    <trd>&c_a;&c_d;&c_h;&c_o;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>steile Stelle</trd>,
    <trd>Steilk&uuml;ste</trd>,
    <trd>steil aufragende K&uuml;ste</trd>,
    <trd>steil aufragender Fels</trd>,
    <trd>steil abfallende K&uuml;ste</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bluff</trd>,
    <trd>cliff</trd>,
    <trd>escarpment</trd>,
    <trd>steep</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>escarpement</trd>,
    <trd>falaise</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd lng="hu">meredek part</trd>,
    <trd>szakad&eacute;k</trd>,
    <trd>mered&eacute;ly</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_u;&c_ch;&c_a;</trd>,
    <trd>&c_o;&c_t;&c_k;&c_o;&c_s;</trd>
  </trdgrp>
</drv>

<drv mrk="krut.inter0ejo">
  <kap>inter<tld/>ejo</kap>
  <snc mrk="krut.inter0ejo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Tre mallar&gcirc;a irejo inter du <tld/>a&jcirc;oj de monto.
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_ja;&c_s;&c_mol;&c_n;&c_ib;&c_n;&c_a;</trd>
  <trd lng="de">Klamm</trd>
  <trd lng="en">defile</trd>
  <trd lng="fr">d&eacute;fil&eacute;<klr> (de montagne)</klr></trd>
  <trdgrp lng="hu">
    <trd>hasad&eacute;k</trd>,
    <trd>szurdok</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_shch;&c_je;&c_l;&c_mol;&c_je;</trd>
</drv>
</art>
