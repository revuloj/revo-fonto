<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ekonom.xml,v 1.20 2023/11/07 19:22:36 revo Exp $">
<kap>
  <rad>ekonom</rad>/o <fnt><bib>ZR</bib></fnt>
</kap>
<drv mrk="ekonom.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="intend.0o.privata">Intendanto<sncref/></ref>:
      <ekz>
        la kanalo apartenas al potenca sinjoro: li estas <tld/>o de la skribisto <klr>[...]</klr> de lia ekscelenco
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        vin salutas Erasto, la <tld/>o de la urbo
        <fnt>
          <bib>NT</bib>
          <lok>Romanoj 16:23</lok>
        </fnt>:
      </ekz>
      <ekz>
        li proponis al &scirc;i plenumi en lia domo la rolon de l' <tld/>ino
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 8a, p. 40a</lok></fnt>.
      </ekz>
    </dif>
    <rim>
      Ne konfuzu kun:
      <refgrp tip="vid">
        <ref cel="ekonom1.0isto">ekonomiisto</ref>,
        <ref cel="ekonom2.0isto">ekonomikisto</ref>.
      </refgrp>
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_k;&c_a;&c_n;&c_o;&c_m;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_r;&c_a;&c_d;&c_ch;&c_y;&c_k;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_a;&c_d;&c_ch;&c_y;&c_k;</trd>
    <trd>&c_ib;&c_n;&c_t;&c_e;&c_n;&c_d;&c_a;&c_n;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ekonom</trd>,
    <trd>hospod&aacute;&rcaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Wirtschafter</trd>,
    <trd>Verwalter</trd>,
    <trd>Gutsverwalter</trd>,
    <trd>Intendant</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bursar</trd>,
    <trd>manager</trd>,
    <trd>steward</trd>,
    <trd>treasurer</trd>,
    <trd>bailiff</trd>,
    <trd>estate manager</trd>
  </trdgrp>
  <trd lng="fa">&f_k;&a_A;&a_r;&f_p;&a_r;&a_d;&a_A;&a_z;</trd>
  <trdgrp lng="fr">
    <trd>&eacute;conome<klr> (subst.)</klr></trd>,
    <trd>intendant</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gazdatiszt</trd>,
    <trd>j&oacute;sz&aacute;gigazgat&oacute;</trd>,
    <trd>tiszttart&oacute;</trd>,
  </trdgrp>
  <trd lng="ja">&#x7BA1;&#x7406;&#x4EBA;  <pr>&#x304B;&#x3093;&#x308A;&#x306B;&#x3093;</pr></trd>
  <trdgrp lng="pt">
    <trd>ec&ocirc;nomo</trd>,
    <trd>mordomo</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_k;&c_o;&c_n;&c_o;&c_m;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_h;&c_o;&c_z;</trd>
  </trdgrp>
  <trd lng="sk">hospod&aacute;rsky odborn&iacute;k</trd>
  <trdgrp lng="zh">
    <trd>&#x7537;&#x7BA1;&#x5BB6; <pr>n&#xE1;ngu&#x1CE;nji&#x101;</pr></trd>,
    <trd>&#x7530;&#x4EA7;&#x7BA1;&#x7406;&#x4EBA; <pr>ti&#xE1;nch&#x1CE;ngu&#x1CE;nl&#x1D0;r&#xE9;n</pr></trd>
  </trdgrp>
</drv>
</art>
