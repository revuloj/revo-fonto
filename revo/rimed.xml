<drv mrk="rimed.antaux0o">
  <kap>anta&ubreve;<tld/>o</kap>
  <snc mrk="rimed.antaux0o.KOMUNE">
    <dif>
      <tld lit="R"/>o por neebligi ion:
      <ekz>
        pri eblaj anta&ubreve;<tld/>oj &scirc;i stre&ccirc;is al si la cerbon, ellaborante, en rapida
        intersekvo, seson da planoj, kaj tuj forlasis ilin kiel malta&ubreve;gajn
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="zorg.antaux0i">anta&ubreve;zorgi</ref>,
      <ref cel="preven.0i">preventi</ref>
    </refgrp>
  </snc>
  <!--
  <trdgrp lng="en">
    <trd>preventative medicine</trd>,
    <trd>precaution</trd>,
    <trd>preventative measure</trd>
  </trdgrp>
  -->
  <trd lng="be">&c_s;&c_r;&c_o;&c_d;&c_a;&c_k;
  <klr>(&c_p;&c_a;&c_p;&c_je;&c_r;&c_a;&c_d;&c_zh;&c_a;&c_l;&c_mol;&c_n;&c_y;,
  &c_p;&c_r;&c_e;&c_v;&c_e;&c_n;&c_t;&c_y;&c_w;&c_n;&c_y;)</klr></trd>
  <trdgrp lng="de">
