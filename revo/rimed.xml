<drv mrk="rimed.cxiu0e">
  <kap>
    &ccirc;iu<tld/>e, <var><kap>&ccirc;ia<tld/>e</kap></var>
  </kap>
  <snc>
    <dif>
      Uzante &ccirc;iun eblan <tld/>on:
      <ekz>
        fanatika pastraro, kiu volas &ccirc;iu<tld/>e elradikigi la kredon al <nom>Jehova</nom>
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiom eble plej rapide kaj &ccirc;ia<tld/>e
        <fnt>
          <bib>MD02</bib>
        </fnt>;
      </ekz>
      <ekz>
        proklami &ccirc;iuflanken kaj &ccirc;iu<tld/>e, ke la entrepreno neniel implicas persekuton
        kontra&ubreve; hebreoj
        <fnt>
          <bib>Monato</bib>
          <aut>Armando Zecchin</aut>
          <vrk><url ref="&Monato;/2008/009377.php">Ne persekutu la hebreojn</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_s;&c_ib;&c_m;&c_ib; <ind>&c_s;&c_r;&c_o;&c_d;&c_k;&c_a;&c_m;&c_ib;</ind></trd>
  <trd lng="de">mit allen <ind>Mittel</ind>n</trd>
  <trd lng="en">through all means</trd>
  <trd lng="id">dengan segala <ind>cara</ind></trd>
  <trd lng="ja">&#x306A;&#x3093;&#x3068;&#x3057;&#x3066;&#x3082;</trd>
  <trd lng="pl">wszelkimi<ind>  &sacute;rodkami</ind></trd>
  <trd lng="uk">&c_u;&c_s;&c_ib;&c_m;&c_a; &c_z;&c_a;&c_s;&c_o;&c_b;&c_a;&c_m;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x60F3;&#x65B9;&#x8A2D;&#x6CD5; <pr>xi&#x1CE;ngf&#x101;ngsh&#xE8;f&#x1CE;</pr></trd>,
    <trd>&#x60F3;&#x65B9;&#x8BBE;&#x6CD5; <pr>xi&#x1CE;ngf&#x101;ngsh&#xE8;f&#x1CE;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rimed.kontraux0o">
  <kap>kontra&ubreve;<tld/>o</kap>
  <snc mrk="rimed.kontraux0o.KOMUNE">
    <dif>
      <ref tip="vid" cel="rimed.0o.KOMUNE"><tld lit="R"/>o</ref>
      por ion kontra&ubreve;i, pribatali, por sin
      <ref tip="vid" cel="sxirm.0i">&scirc;irmi</ref> de &gcirc;i:
      <ekz>
        nepras apliki ta&ubreve;gajn kontra&ubreve;<tld/>ojn por eviti, ke la arteria premo doma&gcirc;u la
        organismon
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2013/011124.php">Atentu la koron</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni tute ne povas scii, kiujn kontra&ubreve;<tld/>ojn ellaborus la celataj
        <klr>[&#x2026;]</klr> problem&scirc;tatoj
        <fnt>
          <aut>Stefan Maul</aut>
            <vrk><url ref="&Monato;/2000/007090.php">Kanajlo-&scirc;tatoj</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 05</lok>
          </fnt>;
      </ekz>
      <ekz>
        okaze de la internacia financa krizo <klr>[&#x2026;]</klr> komerca
        protektismo ne estas bona kontra&ubreve;<tld/>o
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url ref="&CRI;621/2009/08/20/1s101407.htm">
          Estas pli bone...</url></vrk>,
          <lok>2009-08-20</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_r;&c_o;&c_d;&c_a;&c_k;
  <klr>(&c_p;&c_r;&c_o;&c_c;&c_ib;&c_d;&c_z;&c_je;&c_ja;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>
  <trd lng="de">Gegenmittel</trd>
  <trdgrp lng="en">
    <trd>countermeasure</trd>,
    <trd>antidote</trd>
  </trdgrp>
  <trd lng="es">medio de control</trd>
  <trdgrp lng="fr">
    <trd>moyen de lutte</trd>,
    <trd>parade<klr> (protection)</klr></trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>tangkal</baz>penangkal</trd>,
    <trd><baz>tawar</baz>penawar</trd>,
    <trd>pen<ind>cegah</ind></trd>
  </trdgrp>
  <trd lng="nl">tegenmaatregel</trd>
  <trd lng="pl">&sacute;rodek przeciwny</trd>
  <trd lng="uk">&c_k;&c_o;&c_n;&c_t;&c_r;&c_z;&c_a;&c_h;&c_ib;&c_d;</trd>
  <trdgrp lng="zh">
    <trd>&#x4E2D;&#x548C;&#x529B; <pr>zh&#x14D;ngh&#xE9;l&#xEC;</pr></trd>,
    <trd>&#x53CD;&#x5BF9;&#x7684;&#x4E1C;&#x897F; <pr>f&#x1CE;ndu&#xEC;ded&#x14D;ngxi</pr></trd>,
    <trd>&#x53CD;&#x5C0D;&#x7684;&#x6771;&#x897F; <pr>f&#x1CE;ndu&#xEC;ded&#x14D;ngxi</pr></trd>,
    <trd>&#x53CD;&#x4F5C;&#x7528;&#x5242; <pr>f&#x1CE;nzu&#xF2;y&#xF2;ngj&#xEC;</pr></trd>,
    <trd>&#x53CD;&#x4F5C;&#x7528;&#x5291; <pr>f&#x1CE;nzu&#xF2;y&#xF2;ngj&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rimed.viv0o">
  <kap>viv<tld/>o</kap>
  <snc mrk="rimed.viv0o.KOMUNE">
    <dif>
      <tld lit="R"/>o
      por <ref tip="vid" cel="ten.viv0i">vivteni</ref>:
      <ekz>
        liaj viv<tld/>oj <klr>[&#x2026;]</klr> estis donataj al li de la re&gcirc;o &ccirc;iutage dum lia
        tuta vivo
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 25:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i el sia senhaveco &jcirc;etis &ccirc;ion, kion &scirc;i havis, sian tutan viv<tld/>on
        <fnt>
          <bib>NT</bib>
          <lok>Marko 12:44</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi povos liveri al vi viv<tld/>ojn per mia profesio
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tistino kaj kamentubisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li travagadis tiujn samajn urbojn, deklamante siajn versojn, por akiri
        viv<tld/>ojn
        <fnt>
          <bib>Fab3</bib>
          <lok>Dorna vojo de la honoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili havis sufi&ccirc;ajn viv<tld/>ojn, &ccirc;ar ili kontenti&gcirc;is je sia
        modesta sorto
        <fnt><bib>Fab3</bib>, <lok>Historio de unu
           patrino</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_r;&c_o;&c_d;&c_a;&c_k; <klr>(&c_d;&c_l;&c_ja;
  &c_p;&c_a;&c_d;&c_t;&c_r;&c_y;&c_m;&c_k;&c_ib; &c_zh;&c_y;&c_c;&c_mol;&c_c;&c_ja;)</klr></trd>
  <trdgrp lng="de">
    <trd>Lebensunterhalt</trd>,
    <trd>Eink&uuml;nfte</trd>
  </trdgrp><trdgrp lng="en">
    <trd><klr>(one's) </klr>means</trd>,
    <trd>means of existence</trd>,
    <trd><klr>what one does for a </klr>living</trd>
  </trdgrp>
  <trd lng="es">
    medio de subsistencia</trd>
  <trd lng="fr">
    moyen d'existence</trd>
  <trdgrp lng="pl">
    <trd>egzystencja</trd>,
    <trd>&sacute;rodki do &zdot;ycia {pl}</trd>
  </trdgrp>
   <trdgrp lng="zh">
    <trd>&#x751F;&#x8A08; <pr>sh&#x113;ngj&#xEC;</pr></trd>,
    <trd>&#x751F;&#x8BA1; <pr>sh&#x113;ngj&#xEC;</pr></trd>,
    <trd>&#x6D3B;&#x8DEF; <pr>hu&#xF3;l&#xF9;</pr></trd>
  </trdgrp>
</drv>
