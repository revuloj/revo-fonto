<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: legxitim.xml,v 1.9 2024/01/15 16:14:09 revo Exp $">
<kap>
  <rad>le&gcirc;itim</rad>/a
</kap>
<drv mrk="legxitim.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">NEO</uzo>
  <snc mrk="legxitim.0a.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      <ref tip="dif" cel="legx.laux0a">La&ubreve;le&gcirc;a</ref>, konforma al
      la le&gcirc;oj:
      <ekz>
        <tld/>a registaro;
      </ekz><ekz>
        oni tute prave festos la centjari&gcirc;on de UEA en 2008, &ccirc;ar
        la nuna UEA fakte estas la <tld/>a heredanto de la
        organizaj klopodoj de la E<klr>speranto</klr>-movado
        ekde 1908
        <fnt>
          <aut>Jos&eacute; Antonio Vergara</aut>:
          <vrk>Nova jubilea simbolo de UEA</vrk>
          <lok>Libera Folio,
            <url ref="www.liberafolio.org/2007/nova-jubilea-simbolo-de-uea#1356859516992137">2007</url></lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="legxitim.0a.KOMUNE">
    <dif>
      Konforma al logiko, prudento, <ref tip="dif" cel="prav.0a">prava</ref>;
      havanta seriozan pravigon a&ubreve; ka&ubreve;zon:
      <ekz>
        <tld/>a demando;
      </ekz>
      <ekz>
        <tld/>eco pli valoras ol legaleco
        <fnt>
          <vrk><url ref="https://labekbotelo.blogspot.com/2011/05/">Survoje al Kataluna Respubliko: de maig 2011</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u la terposedo estas <tld/>a?
        <fnt>
          <vrk><url ref="&Viki;/Agrara_reformo">Agrara
          reformo</url></vrk>.
          <bib>Viki</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_l;&c_je;&c_g;&c_ib;&c_t;&c_y;&c_m;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>legitim</trd>,
    <trd>berechtigt</trd>
  </trdgrp>
  <trd lng="en">legitimate</trd>
  <trd lng="fr">l&eacute;gitime</trd>
  <trd lng="pl">legitymistyczny</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_l;&c_je;&c_g;&c_i;&c_t;&c_i;&c_m;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="zh">&#x5408;&#x6CD5; <pr>h&#xE9;f&#x1CE;</pr></trd>
</drv>

<drv mrk="legxitim.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="stl">NEO</uzo>
  <snc mrk="legxitim.0isto.POL">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Partiano de re&gcirc;o a&ubreve; tronpretendanto, kies rajtoj bazi&gcirc;as sur
      la rekta heredado:
      <ekz>
        <nom>Ange de L&eacute;on</nom>, <tld/>isto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=475372">Listo
          de la urbestroj de Rennes</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_je;&c_g;&c_ib;&c_t;&c_y;&c_m;&c_ib;&c_s;&c_t;</trd>
  <trd lng="de">Legitimist</trd>
  <trd lng="en">legitimist</trd>
  <trd lng="fr">l&eacute;gitimiste</trd>
  <trd lng="pl">legitymista</trd>     
  <trd lng="ro">monarhist</trd>
  <trd lng="ru">&c_l;&c_je;&c_g;&c_i;&c_t;&c_i;&c_m;&c_i;&c_s;&c_t;</trd>
</drv>

</art>

