<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>fosfit</rad>/o
</kap>
<drv mrk="fosfit.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="super" cel="sal.0o.KEM">Salo</ref> de <tld/>a acido.  
    </dif>
    <trd lng="de">Phosphit</trd>
    <trd lng="fr">phosphite</trd>
    <trd lng="ru">&c_f;&c_o;&c_s;&c_f;&c_i;&c_t;</trd>
  </snc>
  <snc>
    <uzo tip="fak">MIN</uzo>
    <fnt><bib>PV</bib></fnt>
    <uzo tip="stl">ARK</uzo>
    <ref tip="dif" cel="fosfor2.0o">fosforito.</ref>  
  </snc>
</drv>
<drv mrk="fosfit.0aacido">
  <kap><tld/>a acido</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="acid.0o">Acido</ref> el
      <ref cel="fosfor.0o">fosforo</ref>. H<sub>3</sub>PO<sub>3</sub>.
    </dif>
  </snc>
  <trd lng="fr">acide phosphoreux</trd>
</drv>
</art>

