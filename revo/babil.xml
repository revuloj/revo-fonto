  <trd lng="he">&lamed;&#x5B0;&shin;&#x5C2;&vav;&#x5B9;&het;&#x5B5;&het;&#x5B7;</trd>
  <trd lng="ja">&#x307A;&#12385;&#12419;&#x304F;&#12385;&#12419;&#x8A71;&#12377;  <pr>&#x307A;&#12385;&#12419;&#x304F;&#12385;&#12419;&#x306F;&#x306A;&#12377;</pr></trd>
