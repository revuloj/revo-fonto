  <trd lng="bg">&c_b;&c_malmol;&c_b;&c_r;&c_ja;</trd>
  <trd lng="bo">&t_kha;&t_tsheg;&t_ba;&t_da;&t_ra;&t_tsheg;&t_ga;&t_lata;&t_naro;&t_da;&t_tsheg;</trd>
