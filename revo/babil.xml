<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: babil.xml,v 1.41 2022/03/12 18:02:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>babil</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="babil.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="babil.0i.familiare">
    <dif>
      Intime, familiare interparoli:
      <ekz>
        la senzorge tra<tld/>itaj (pasigitaj
        <tld/>ante) horoj en la drinkejo.
      </ekz>
    </dif>
    <trd lng="hu">cseveg</trd>
  </snc>
  <snc mrk="babil.0i.babilacxi">
     <dif>
       <ref tip="dif" cel="babil.0acxi.KOMUNE"><tld lit="B"/>a&ccirc;i</ref>:
       <ekz>
         li <tld/>as anstata&ubreve; labori;
       </ekz>
       <ekz>
         kiu multe <tld/>as, pensas malmulte<fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         <tld/>adi teorian sensenca&jcirc;on        
         <fnt>
           <bib>EE</bib>
         </fnt>;
       </ekz>
       <ekz>
         ne timu tran&ccirc;ilon, timu
         <tld/>on<fnt><bib>PrV</bib></fnt>.
       </ekz>
     </dif>
     <trdgrp lng="ca">
       <trd>garlar</trd>,
       <trd>parlotejar</trd>
     </trdgrp>
     <trdgrp lng="en">
       <trd>babble</trd>
     </trdgrp>
     <trdgrp lng="fa">
       <trd>&a_w;&a_r;&a_A;&a_j;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>,
       <trd>&f_p;&a_r;&a_H;&a_r;&a_f;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
     </trdgrp>
     <trdgrp lng="it">
     <trd>cianciare</trd>, <trd>blaterare</trd>
     </trdgrp>
     <trd lng="nl">kletsen</trd>
     <trd lng="no">bable</trd>
     <trdgrp lng="pl">
       <trd>gada&cacute;</trd>,
       <trd>gl&eogonek;dzi&cacute;</trd>,
       <trd>nawija&cacute;<klr> (pot.)</klr></trd>,
       <trd>klecha&cacute;<klr> (pot.)</klr></trd>,
       <trd>marudzi&cacute;</trd>,
       <trd>zrz&eogonek;dzi&cacute;</trd>
     </trdgrp>
     <trd lng="sv">babbla</trd>
   </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_u;&c_t;&c_a;&c_r;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_a;&c_m;&c_a;&c_n;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_a;&c_l;&c_b;&c_a;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_l;&c_a;&c_p;&c_a;&c_t;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_b;&c_malmol;&c_b;&c_r;&c_ja;</trd>
  <trd lng="bo">&#xF41;&#xF0B;&#xF56;&#xF51;&#xF62;&#xF0B;&#xF42;&#xFB3;&#xF7C;&#xF51;&#xF0B;</trd>
  <trdgrp lng="br">
    <trd>fistilha&ntilde;</trd>,
    <trd>flapa&ntilde;</trd>,
    <trd>klakenni&ntilde;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>xerrar</trd>,
    <trd>rallar</trd>,
    <trd>clacar</trd>,
    <trd>garlar</trd>,
    <trd>parlotejar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>besedovat</trd>,
    <trd>pov&iacute;dat si</trd>,
    <trd>tlachat</trd>,
    <trd>&rcaron;&iacute;kat bezobsa&zcaron;n&eacute; banality</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>quatschen</trd>,
    <trd>schwatzen</trd>,
    <trd>plaudern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>chat</trd>,
    <trd>chit-chat</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>conversar</trd>,
    <trd>platicar</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&f_g;&f_p; &a_z;&a_d;&a_n;</trd>,
    <trd>&a_w;&a_r;&a_A;&a_j;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&f_p;&a_r;&a_H;&a_r;&a_f;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bavarder</trd>,
    <trd>papoter</trd>,
    <trd>causer</trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5E4;&#x5D8;&#x5E4;&#x5D8;</trd>
  <trdgrp lng="hu">
    <trd>cseveg</trd>,
    <trd>fecseg</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>cianciare</trd>,
    <trd>chiaccherare</trd>,
    <trd>blaterare</trd>
    </trdgrp>
  <trdgrp lng="nl">
    <trd>kletsen</trd>,
    <trd>babbelen</trd>
  </trdgrp>
  <trdgrp lng="no">
    <trd>bable</trd>,
    <trd>sm&aring;prate</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>gada&cacute;</trd>
    <trd>gaw&eogonek;dzi&cacute;</trd>,
    <trd>gwarzy&cacute;</trd>,
    <trd>gl&eogonek;dzi&cacute;</trd>,
    <trd>nawija&cacute;<klr> (pot.)</klr></trd>,
    <trd>klecha&cacute;<klr> (pot.)</klr></trd>,
    <trd>marudzi&cacute;</trd>,
    <trd>papla&cacute;</trd>
    <trd>zrz&eogonek;dzi&cacute;</trd>
  </trdgrp>
  <trd lng="pt">tagarelar</trd>
  <trd lng="ru">&c_b;&c_o;&c_l;&c_t;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>mnoho rozpr&aacute;va&tcaron;</trd>,
    <trd>t&aacute;ra&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>babbla</trd>,
    <trd>prata</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x431;&#x435;&#x441;&#x456;&#x434;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x431;&#x430;&#x43B;&#x430;&#x43A;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="babil.0acxi">
  <kap><tld/>a&ccirc;i</kap>
  <snc mrk="babil.0acxi.KOMUNE">
    <dif>
      Tro multe paroli, pri negravaj, pri ne bone sciataj aferoj:
      <ekz>
        jen vi nehaltigeble <tld/>a&ccirc;is dum Fortunato simple staris
        kaj 
        a&ubreve;skultadis
        <fnt><aut>A. L&ouml;wenstein</aut>, <vrk>La &scirc;tona
        urbo</vrk>, 
        <lok>FEL, 1999</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="klacx.0i">kla&ccirc;i</ref>,
      <ref cel="lang.0o">lango</ref>,
      <ref cel="muel.0i">mueli</ref>,
      <ref cel="pep.0i">pepi</ref>
    </refgrp>
    <trdgrp lng="ca">
      <trd>garlar</trd>,
      <trd>parlotejar</trd>
    </trdgrp>
    <trd lng="es">cotillear</trd>
    <trdgrp lng="fa">
      <trd>&a_w;&a_r;&a_A;&a_j;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>,
      <trd>&f_p;&a_r;&a_H;&a_r;&a_f;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
    </trdgrp>
    <trd lng="fr">cancaner<klr> (dire des cancans)</klr></trd>
    <trdgrp lng="it">
    <trd>cianciare</trd>, <trd>blaterare</trd>
    </trdgrp>
    <trd lng="nl">kletsen</trd>
    <trd lng="no">bable</trd>
    <trdgrp lng="pl">
      <trd>gada&cacute;</trd>,
      <trd>gl&eogonek;dzi&cacute;</trd>,
      <trd>nawija&cacute;<klr> (pot.)</klr></trd>,
      <trd>klecha&cacute;<klr> (pot.)</klr></trd>,
      <trd>marudzi&cacute;</trd>,
      <trd>zrz&eogonek;dzi&cacute;</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>babbla</trd>,
      <trd>svamla</trd>,
      <trd>tjattra</trd>
    </trdgrp>
    <trdgrp lng="uk">
      <trd>&c_t;&c_je;&c_r;&c_je;&c_v;&c_je;&c_n;&c_i;&c_t;&c_i;</trd>,
      <trd>&c_v;&c_a;&c_r;&c_n;&c_ja;&c_k;&c_a;&c_t;&c_i;</trd>,
      <trd>&c_b;&c_a;&c_z;&c_ib;&c_k;&c_a;&c_t;&c_i;</trd>,
      <trd>&c_t;&c_o;&c_ch;&c_i;&c_t;&c_i; &c_l;&c_ja;&c_s;&c_i;</trd>
    </trdgrp>    
  </snc>
  <trd lng="he">&#x5DC;&#x5B0;&#x5E9;&#x5C2;&#x5D5;&#x5B9;&#x5D7;&#x5B5;&#x5D7;&#x5B7;</trd>
</drv>

<drv mrk="babil.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="babil.0ejo.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <ref tip="super" cel="ret.0ejo.KOMP">Retejo</ref> en kiu pluraj  
      homoj povas realtempe interskribi:
      <ekz>
        eniru la <tld/>ejon se vi volas diskuti kun aliaj homoj pri 
        diversaj temoj
        <fnt>
          <aut>G. Denes</aut>,
          <vrk><url ref="http://www.ludanto.org/babilanto/">
          ludanto.org/babilejo</url></vrk>,
          <lok>[vidita en 2008]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_ch;&c_a;&c_t;</trd>
    <trd lng="ca">xat</trd>
    <trd lng="es">foro<klr tip="amb"> (internet)</klr></trd>
    <trd lng="fa">&f_g;&f_p;&f_g;&a_A;&a_h;</trd>
    <trd lng="fr">salon<klr> (internet)</klr></trd>
    <trdgrp lng="it">
    <trd>forum<klr> (informatico)</klr></trd>, <trd>chat-room</trd>
    </trdgrp>
    <trd lng="nl">chatstek</trd>
    <trd lng="no">chattested</trd>
    <trdgrp lng="pl">
      <trd>czat</trd>,
      <trd>pogaw&eogonek;dka</trd>
    </trdgrp>
    <trd lng="sv">chatt</trd>
  </snc>
  <trd lng="cs">IRC kan&aacute;l</trd>
  <trd lng="he">&#x5E4;&#x5D5;&#x5E8;&#x5D5;&#x5DD; &#x5DC;&#x5E9;&#x5D9;&#x5D7;&#x5D4;</trd>
  <trd lng="sk">IRC kan&aacute;l</trd>
</drv>

<drv mrk="babil.0ema">
  <kap><ofc>*</ofc><tld/>ema</kap>
  <snc mrk="babil.0ema.KOMUNE">
    <dif>
      Inklina al ofta, abunda interparolado:
      <ekz>
        &scirc;i estas anka&ubreve; tre <tld/>ema kaj kriema
        <fnt><bib>F</bib>, <lok>&FE; 41</lok></fnt>;
      </ekz>
      <ekz>
        oni trinkis <klr>[...]</klr> sufi&ccirc;e por meti en movi&gcirc;adon la
        <tld/>eman bu&scirc;on
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la princo malamis <klr>[&scirc;in]</klr> pro &scirc;ia <tld/>emo kaj
        malhonestaj faroj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>wygadany</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_l;&c_b;&c_a;&c_t;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_g;&c_a;&c_v;&c_a;&c_r;&c_k;&c_ib;</trd>,
    <trd>&c_g;&c_a;&c_m;&c_a;&c_n;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_ja;&c_z;&c_y;&c_ch;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>xerraire</trd>,
    <trd>parlador<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pov&iacute;dav&yacute;</trd>,
    <trd>ukecan&yacute;</trd>
  </trdgrp>
  <trd lng="es">charlat&aacute;n<klr> (adj.)</klr></trd>
  <trdgrp lng="fa">
    <trd>&a_w;&a_r;&a_A;&a_j;</trd>,
    <trd>&f_p;&a_r;&a_H;&a_r;&a_f;</trd>
  </trdgrp>
  <trd lng="fr">bavard<klr> (adj.)</klr></trd>
  <trd lng="he">&#x5D0;&#x5D5;&#x5D4;&#x5D1; &#x5E9;&#x5D9;&#x5D7;&#x5D4;</trd>
  <trdgrp lng="it">
    <trd>ciarliero</trd>, <trd>loquace</trd>
    </trdgrp>
  <trd lng="nl">babbelziek</trd>
  <trd lng="sk">uvraven&yacute;</trd>
  <trd lng="sv">pratglad</trd>
  <trdgrp lng="uk">
    <trd>&#x431;&#x430;&#x43B;&#x430;&#x43A;&#x443;&#x447;&#x438;&#x439;</trd>,
    <trd>&#x433;&#x43E;&#x432;&#x456;&#x440;&#x43A;&#x438;&#x439;</trd>,
    <trd>&#x44F;&#x437;&#x438;&#x43A;&#x430;&#x442;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="babil.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="babil.0ulo.KOMUNE">
    <dif>
      Homo inklina al multa parolado:
      <ekz>
        tie estis kafejo, kie ordinare multaj <tld/>uloj kaj
        nova&jcirc;portantoj kolekti&gcirc;is
        <fnt>
          <aut>S. Engholm</aut>,
          <vrk>Homoj sur la tero</vrk>,
          <lok>1932</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>cotorra</trd>,
      <trd>xerraire<klr> (subst.)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>gadu&lstroke;a</trd>,
      <trd>gaw&eogonek;dziarz</trd>,
      <trd>papla</trd>
      <trd>maruda</trd>,
      <trd>zrz&eogonek;da</trd>
    </trdgrp>
    <trd lng="ro">vorbitor</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_o;&c_l;&c_b;&c_a;&c_t;</trd>,
    <trd>&c_b;&c_a;&c_l;&c_b;&c_a;&c_t;&c_u;&c_n;</trd>,
    <trd>&c_l;&c_a;&c_p;&c_a;&c_t;&c_u;&c_n;</trd>,
    <trd>&c_t;&c_a;&c_r;&c_a;&c_t;&c_u;&c_n;</trd>
  </trdgrp>
  <trd lng="cs">tlachal</trd>
  <trd lng="es">charlat&aacute;n</trd>
  <trdgrp lng="fa">
    <trd>&a_w;&a_r;&a_A;&a_j;</trd>,
    <trd>&f_p;&a_r;&a_H;&a_r;&a_f;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bavard<klr> (subst.)</klr></trd>,
    <trd>piplette</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>chiacchierone</trd>, <trd>ciarlone</trd>
    </trdgrp>
  <trd lng="nl">babbelaar</trd>
  <trd lng="sk">t&aacute;raj</trd>
  <trd lng="sv">pratkvarn</trd>
</drv>

<drv mrk="babil.ret0ejo">
  <kap>ret<tld/>ejo</kap>
  <snc mrk="babil.ret0ejo.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <ref tip="dif" cel="babil.0ejo.KOMP">Babilejo <sncref/></ref>:
      <ekz>
        plej oftaj vizitantoj de ret<tld/>ejoj estas gejunuloj
        <fnt>
          <aut>A. &Ccaron;i&zcaron;ikait&#x117;</aut>,
          <vrk>Uzo de nomoj en retbabilejoj</vrk>,
          <lok>en: Lingvo kaj interreto, 2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">xat</trd>
    <trd lng="fa">&f_g;&f_p;&f_g;&a_A;&a_h;</trd>
    <trd lng="fr">salon<klr> (internet)</klr></trd>
    <trdgrp lng="it">
    <trd>forum<klr> (informatico)</klr></trd>, <trd>chat-room</trd>
    </trdgrp>
    <trd lng="nl">chatstek</trd>
    <trd lng="no">chattested</trd>
    <trdgrp lng="pl">
      <trd>czat</trd>,
      <trd>pogaw&eogonek;dka</trd>
    </trdgrp>
    <trd lng="sv">chatt</trd>
  </snc>
</drv>
</art>
