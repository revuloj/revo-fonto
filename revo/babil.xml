<drv mrk="babil.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="babil.0i.familiare">
    <dif>
      Intime, familiare interparoli:
      <ekz>
        la senzorge tra<tld/>itaj (pasigitaj
        <tld/>ante) horoj en la drinkejo.
      </ekz>
    </dif>
    <trd lng="hu">cseveg</trd>
    <trd lng="zh">&#x804A;&#x804A; <pr>li&#xE1;oliao</pr></trd>
</snc>
  <snc mrk="babil.0i.babilacxi">
     <dif>
       <ref tip="dif" cel="babil.0acxi.KOMUNE"><tld lit="B"/>a&ccirc;i</ref>:
       <ekz>
         li <tld/>as anstata&ubreve; labori;
       </ekz>
       <ekz>
         kiu multe <tld/>as, pensas malmulte<fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         <tld/>adi teorian sensenca&jcirc;on
         <fnt>
           <bib>EE</bib>
         </fnt>;
       </ekz>
       <ekz>
         ne timu tran&ccirc;ilon, timu
         <tld/>on<fnt><bib>PrV</bib></fnt>.
       </ekz>
     </dif>
     <trdgrp lng="ca">
       <trd>garlar</trd>,
       <trd>parlotejar</trd>
     </trdgrp>
     <trdgrp lng="en">
       <trd>babble</trd>
     </trdgrp>
     <trdgrp lng="fa">
       <trd>&a_w;&a_r;&a_A;&a_j;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>,
       <trd>&f_p;&a_r;&a_H;&a_r;&a_f;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
     </trdgrp>
     <trdgrp lng="it">
     <trd>cianciare</trd>, <trd>blaterare</trd>
     </trdgrp>
     <trd lng="nl">kletsen</trd>
     <trd lng="no">bable</trd>
     <trdgrp lng="pl">
       <trd>gada&cacute;</trd>,
       <trd>gl&eogonek;dzi&cacute;</trd>,
       <trd>nawija&cacute;<klr> (pot.)</klr></trd>,
       <trd>klecha&cacute;<klr> (pot.)</klr></trd>,
       <trd>marudzi&cacute;</trd>,
       <trd>zrz&eogonek;dzi&cacute;</trd>
     </trdgrp>
     <trd lng="sv">babbla</trd>
     <trd lng="zh">&#x4E71;&#x4F83; <pr>lu&#xE0;nk&#x1CE;n</pr></trd>
</snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_u;&c_t;&c_a;&c_r;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_a;&c_m;&c_a;&c_n;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_a;&c_l;&c_b;&c_a;&c_t;&c_a;&c_c;&c_mol;</trd>,
