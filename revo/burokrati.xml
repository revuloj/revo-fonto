<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: burokrati.xml,v 1.13 2025/04/16 22:00:21 revo Exp $">
<kap>
  <rad>burokrati</rad>/o
</kap>

<drv mrk="burokrati.0o">
  <kap><tld/>o</kap>
  <snc mrk="burokrati.0o.burokrataro">
    <dif>
      <ref tip="dif" cel="burokr.0aro.POL">Burokrataro</ref>:
      <ekz>
        &scirc;tupoj kondukantaj al gloro kaj profito en la <tld/>o
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 73a, volumo 2a, p.
        533a</lok></fnt>;
      </ekz>
      <ekz>
        en &ccirc;iu lando, &ccirc;iu politika sistemo ekzistas burokratoj kaj <tld/>oj spegulantaj
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2008/009402.php">Socipolitika satiro</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_ju;&c_r;&c_a;&c_k;&c_r;&c_a;&c_t;&c_y;&c_ja;</trd>
    <trd lng="ca">burocr&agrave;cia <klr tip="amb">(conjunt de funcionaris)</klr></trd>
    <trd lng="zh">&#23462;&#28023; <pr>hu&agrave;nh&#462;i</pr></trd>
</snc>
  <snc mrk="burokrati.0o.burokratismo">
    <dif>
      <ref tip="dif" cel="burokr.0ismo.POL">Burokratismo</ref>:
      <ekz>
        tio estas gastigado &#8208; sed gastigado kiu memorigas pli la traktadon
        de ekster&scirc;tatano fare de &scirc;tata <tld/>o ol hejmecan &ccirc;esidon
        <fnt>
        <aut>&Ccirc;ilia Esperantista Junurala Organizo</aut>,
        <vrk><url ref="http://chejo.blogspot.com/">[blogo]</url></vrk>,
        <lok>2006-01-11</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;iam kreskanta EU-<tld/>o tre &gcirc;enas
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2013/011120.php">Anka&ubreve; paco konas siajn (land)limojn</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_ju;&c_r;&c_a;&c_k;&c_r;&c_a;&c_t;&c_y;&c_z;&c_m;</trd>
    <trdgrp lng="ca">
      <trd>burocratisme</trd>,
      <trd>burocr&agrave;cia</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#20844;&#21153;&#21592;&#20316;&#39118; <pr>g&#333;ngw&ugrave;yu&aacute;nzu&ograve;f&emacron;ng</pr></trd>,
    <trd>&#23448;&#20698;&#25919;&#27835; <pr>gu&amacron;nli&aacute;ozh&egrave;ngzh&igrave;</pr></trd>,
    <trd>&#24418;&#24335;&#20027;&#20041; <pr>x&iacute;ngsh&igrave;zh&#468;y&igrave;</pr></trd>,
    <trd>&#23448;&#20698;&#20027;&#20041; <pr>gu&amacron;nli&aacute;ozh&#468;y&igrave;</pr></trd>,
    <trd>&#23448;&#20698;&#21046; <pr>gu&amacron;nli&aacute;ozh&igrave;</pr></trd>,
    <trd>&#31185;&#23618;&#21046; <pr>k&emacron;c&eacute;ngzh&igrave;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="bg">&c_b;&c_ju;&c_r;&c_o;&c_k;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trd lng="cs">byrokracie</trd>
  <trd lng="de">B&uuml;rokratie</trd>
  <trd lng="en">bureaucracy</trd>
  <trd lng="fr">bureaucratie</trd>
  <trd
  lng="he">&bet;&#1468;&#1460;&yod;&resh;&vav;&#1465;&qof;&resh;&#1463;&tet;&#1460;&yod;&#1464;&he;</trd>
  <trd lng="hr">birokracija</trd>
  <trd lng="nl">bureaucratie</trd>
  <trd lng="pl">biurokracja</trd>
  <trd lng="pt">burocracia</trd>
  <trd lng="ru">&c_b;&c_ju;&c_r;&c_o;&c_k;&c_r;&c_a;&c_t;&c_i;&c_ja;</trd>
  <trd lng="sk">byrokracia</trd>
  <trd lng="sr">&c_b;&c_i;&c_r;&c_o;&c_k;&c_r;&c_a;&c_t;&c_i;&#1112;&c_a;</trd>
  <trd lng="uk">&c_b;&c_ju;&c_r;&c_o;&c_k;&c_r;&c_a;&c_t;&c_ib;&c_ja;</trd>
</drv>
