<drv mrk="hund.0ejo">
  <kap><tld/>ejo</kap>
    <snc mrk="hund.0ejo.KOMUNE">
      <dif>
      Loko speciale aran&gcirc;ita por ke vivu eksterdome kaj
      malliberece la hundoj:
     <ekz>
       &ccirc;i-nokte mi sidis sur la &scirc;tiparo apud la <tld/>ejo
       kaj amuzis min inciteti la alligitan <tld/>on
       <fnt>
         <aut>H. C. Andersen, trad. K. Knopf</aut>,
         <vrk><url
         ref="http://donh.best.vwh.net/Esperanto/Literaturo/Noveloj/Fabeloj_de_Andersen/KoboldetoH.html">
         Koboldeto kaj sinjorino</url></vrk>,
       </fnt>;
     </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        dando el re&gcirc;a <tld/>ejo
        <fnt><bib>Prv</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>gossera</trd>,
    <trd>canera</trd>
  </trdgrp>
  <trd lng="cs">ps&iacute; bouda</trd>
  <trd lng="en">kennel</trd>
  <trd lng="es">caseta del perro</trd>
  <trd lng="fa">&a_l;&a_A;&a_n;&a_h;&a_hamza_sure;
  <ind>&a_s;&f_g;</ind></trd>
  <trd lng="fr">chenil</trd>
  <trd lng="id"><ind>kandang</ind> anjing</trd>
  <trd lng="it">canile</trd>
  <trdgrp lng="pl">
    <trd>buda dla <ind>psa</ind></trd>,
    <trd>psia buda</trd>,
    <trd>psiarnia</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>cub&abreve; pentru c&acirc;ini</trd>,
    <trd>casa de c&acirc;ine</trd>
  </trdgrp>
  <trd lng="sk">psia b&uacute;da</trd>
  <trd lng="tr">k&ouml;pek kul&uuml;besi</trd>
  <trd lng="uk">&c_p;&c_s;&c_a;&c_r;&c_n;&c_ja;</trd>
</drv>

<drv mrk="hund.cxas0o">
  <kap>&ccirc;as<tld/>o</kap>
  <snc mrk="hund.cxas0o.ZOO">
    <dif>
      <ref tip="super" cel="hund.0o.dombesto"><tld lit="H"/>o</ref>
      uzata en &ccirc;aso por trovo, persekuto, mortigo a&ubreve; alporto de bestoj:
      <ekz>
        li balda&ubreve; renkontis ku&scirc;antan &ccirc;as<tld/>on, kiu
        spiregis kiel iu, kiu laci&gcirc;is de la kurado
        <fnt><bib>ElektFab</bib>, <lok>la bremenaj muzikistoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ja;&c_w;&c_n;&c_ib;&c_ch;&c_y; &c_s;&c_a;&c_b;&c_a;&c_k;&c_a;</trd>
  <trdgrp lng="br">
    <trd>ki-chase</trd>,
    <trd>ki-hemolc'h</trd>
  </trdgrp>
  <trd lng="ca">gos de ca&ccedil;a</trd>
  <trd lng="de">Jagdhund</trd>
  <trdgrp lng="en">
    <trd>hunting dog</trd>,
    <trd>hunting hound</trd>
  </trdgrp>
  <trd lng="es">perro de caza</trd>
  <trd lng="fa">&a_s;&f_g; &a_sh;&f_k;&a_A;&a_r;&f_y;</trd>
  <trd lng="fr">chien de chasse</trd>
  <trd lng="hu">vad&aacute;szkutya</trd>
  <trd lng="id"><ind>anjing</ind> pemburu</trd>
  <trd lng="it"><ind>cane</ind> da caccia</trd>
  <trd lng="nl">jachthond</trd>
  <trdgrp lng="pl">
    <trd>pies go&nacute;czy</trd>,
    <trd>pies <ind>my&sacute;liwski</ind></trd>
  </trdgrp>
  <trd lng="pt">c&atilde;o de ca&ccedil;a</trd>
  <trd lng="ro">c&acirc;ine de v&acirc;n&abreve;toare</trd>
  <trd lng="ru">&c_o;&c_h;&c_o;&c_t;&c_n;&c_i;&c_ch;&c_mol;&c_ja; &c_s;&c_o;&c_b;&c_a;&c_k;&c_a;</trd>
  <trd lng="sv">jakthund</trd>
  <trdgrp lng="tr">
    <trd>av k&ouml;pe&gbreve;i</trd>
  </trdgrp>
  <trd lng="uk">&c_m;&c_i;&c_s;&c_l;&c_i;&c_v;&c_s;&c_mol;&c_k;&c_i;&c_j; &c_s;&c_o;&c_b;&c_a;&c_k;&c_a;</trd>
</drv>

<drv mrk="hund.Cxas0oj">
  <kap>&Ccirc;as<tld/>oj
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
