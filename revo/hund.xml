<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>hund</rad>/o
</kap>
<drv mrk="hund.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="hund.0o.genro">
    <dif>
      Genro el familio <ref tip="super" cel="hunded.0oj"><tld/>edoj</ref>
      (<trd lng="la">Canis</trd>).
    </dif>
  </snc>
  <snc mrk="hund.0o.dombesto">
    <dif>
      Dombesto apartenanta al tiu genro, devenanta de
      lupo, kun multaj rasoj, uzata precipe por gardado kaj &ccirc;asado:
      <ekz>
        li promenas kun tri <tld/>oj
        <fnt><bib>F</bib>, <lok>&FE; 12</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>o estas plej bona amiko de homo;
      </ekz>
      <ekz>
        <tld/>o gardanta du pordojn restos malsata<fnt>Z</fnt>
        <klr>(oni ne povas servi du mastrojn samtempe)</klr>;
      </ekz>
      <ekz>
        bojas <tld/>o sen puno e&ccirc; kontra&ubreve; la suno
        <klr>(tion oni diras al iu, kiu miras, ke oni lin kritikas)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>oni lin konas kiel makulharan <tld/>on</ind>
        <klr>(li estas de &ccirc;iuj konata)</klr>
        <trd lng="fr">il est <ind>connu</ind> comme le loup blanc</trd>
        <fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>o bonrasa estas bona por &ccirc;aso
        <klr>(li havas la kvalitojn a&ubreve; la mankojn de siaj parencoj
        a&ubreve; avoj)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        mordantaj <tld/>oj kuras &ccirc;iam kun vundoj
        <klr>(malpaculon &ccirc;iu batas)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>bojanta <tld/>o ne mordas</ind>
        <klr>(kiu multe minacas, ne estas la plej dan&gcirc;era)</klr>
        <trd lng="fr">chien qui aboie ne mord pas</trd>
        <fnt>Z</fnt>;
      </ekz>
      <ekz>
        ne timu <tld/>on bojantan, timu <tld/>on silentan
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>por <tld/>on dronigi oni nomas &gcirc;in rabia</ind>
        <klr>(oni &ccirc;iam trovas pretekston por malutili malamaton)</klr>
        <trd lng="fr">pour noyer son chien on lui trouve la
        rage</trd>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="hund.dom0o">dom<tld/>o</ref>
  </snc>
  <trd lng="ar">&kaf1;&fatha;&lam;&sukun;&ba;&dammatan; &jim; 
&kaf1;&kasra;&lam;&fatha_alif;&ba;&dammatan;</trd>
  <trd lng="be">&c_s;&c_a;&c_b;&c_a;&c_k;&c_a;</trd>   
  <trd lng="br">ki</trd>
  <trd lng="ca">gos</trd>
  <trd lng="cs">pes</trd>
  <trd lng="de">Hund</trd>
  <trd 
lng="el">&sigma;&kappa;&ypsilon_ton;&lambda;&omikron;&sigma_fina;</trd>
  <trd lng="en">dog</trd>
  <trd lng="es">perro</trd>
  <trd lng="fr">chien</trd>
  <trd lng="hu">kutya</trd>
  <trd lng="it">cane</trd>
  <trd lng="nl">hond</trd>
  <trd lng="pl">pies</trd>
  <trdgrp lng="pt">
    <trd>cachorro</trd>,
    <trd>c&atilde;o</trd>
  </trdgrp>
  <trd lng="sk">pes</trd>
  <trd lng="sv">hund</trd>
  <trd lng="ru">&c_s;&c_o;&c_b;&c_a;&c_k;&c_a;</trd>
  <trd lng="tr">k&ouml;pek</trd>
  <trd lng="vo">dog</trd>
</drv>

<drv mrk="hund.0a">
  <kap><tld/>a</kap>
  <snc mrk="hund.0a.deHundo">
    <dif>
      De <tld/>o:
      <ekz>
        <tld/>a mordo.
      </ekz>
    </dif>
    <trd lng="ca">can&iacute;</trd>
    <trd lng="cs">ps&iacute;</trd>
    <trd lng="en">canine</trd>
    <trd lng="hu">kutya-</trd>
  </snc>
  <snc mrk="hund.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Mizera; konvena por besto, ne por homo:
      <ekz>
        <ind><tld/>a vivo</ind>
        <trd lng="fr">une <ind>vie</ind> de chien</trd>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="hu">kutya<klr tip="ind"> (&aacute;tv.)</klr></trd>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_b;&c_a;&c_ch;&c_y;</trd>
  <trd lng="br">-<ind>ki</ind></trd>
  <trd 
lng="el">&sigma;&kappa;&ypsilon;&lambda;&jota_ton;&sigma;&jota;&omikron;&sigma_fina;</trd>
  <trdgrp lng="fr">
    <trd>canin</trd>,
    <trd>de <ind>chien</ind></trd>
  </trdgrp>
  <trd lng="nl">honds</trd>
  <trd lng="pl">psi</trd>
  <trd lng="pt">canino</trd>
  <trd lng="ru">&c_s;&c_o;&c_b;&c_a;&c_ch;&c_i;&c_j;</trd>
  <trd lng="sk">ps&iacute;</trd>
</drv>

<drv mrk="hund.cxas0o">
  <kap>&ccirc;as<tld/>o</kap>
  <snc mrk="hund.cxas0o.ZOO">
    <dif>
      <ref tip="super" cel="hund.0o.dombesto"><tld lit="H"/>o</ref> uzata
      en
      &ccirc;aso por trovo, persekuto, mortigo a&ubreve; alporto de bestoj:
      <ekz>
        li balda&ubreve; renkontis ku&scirc;antan &ccirc;as<tld/>on, kiu 
spiregis kiel iu,
        kiu laci&gcirc;is de la kurado
        <fnt><bib>ElektFab</bib>, <lok>la bremenaj muzikistoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>        
  <trdgrp lng="br">     
    <trd>ki-chase</trd>
    <trd>ki-hemolc'h</trd>
  </trdgrp>
  <trd lng="de">Jagdhund</trd>
  <trd lng="fr">chien de chasse</trd>
  <trd lng="pt">c&atilde;o de ca&ccedil;a</trd>
</drv>

<drv mrk="hund.&Ccirc;as0oj">
  <kap>&Ccirc;as<tld/>oj</kap>
  <mlg>CVn</mlg>
  <snc>
    <dif>
      Nord-hemisfera stelfiguro (<trd lng="la">Canes Venatici</trd>).
    </dif>
  </snc>
  <trd lng="de">Jagdhunde</trd>
  <trd lng="en">Canes Venatici</trd>
  <trd lng="es">Los <ind>Perros</ind> de Caza</trd>
</drv>

<drv mrk="hund.cxen0o">
  <kap>&ccirc;en<tld/>o</kap>
  <snc mrk="hund.cxen0o.ZOO">
    <dif>
      <ref tip="super" cel="hund.0o.dombesto"><tld lit="H"/>o</ref> kiu
      gardas domon a&ubreve; pasejon, tenate per &ccirc;eno:
      <ekz>
        krom tio trovi&gcirc;as anka&ubreve; &ccirc;en<tld/>o, kiu bojas 
kontra&ubreve; &ccirc;io kaj &ccirc;iu
        <fnt><bib>Fab4</bib>, <lok>kion la patro faras, estas &ccirc;iam
        &gcirc;usta</lok></fnt>.
      </ekz>
    </dif>
  </snc>                                               
  <trd lng="br">ki-porzh</trd>
  <trd lng="fr">chien de garde<klr tip="amb"> (&agrave; 
l'attache)</klr></trd>
  <trd lng="pt">c&atilde;o de guarda</trd>
</drv>

<drv mrk="hund.dom0o">
  <kap>dom<tld/>o</kap>
  <snc mrk="hund.dom0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="hund.0o.dombesto"><tld lit="H"/>o
    <sncref ref="hund.0o.dombesto"/></ref>
    <ref tip="super" cel="hund.0o.genro">hundo <sncref
    ref="hund.0o.genro"/></ref>
  </snc>
</drv>

<drv mrk="hund.lepor0o">
  <kap>lepor<tld/>o</kap>
  <snc mrk="hund.lepor0o.ZOO">
    <dif>
      <ref tip="super" cel="hund.0o.dombesto"><tld lit="H"/>o</ref> el raso
      rapidkura, gracie maldika, kapabla kurkapti leporon.
    </dif>
  </snc>
  <trd 
lng="be">&c_g;&c_a;&c_n;&c_ch;&c_a;&c_k;</trd>                           
  <trd lng="br">levran</trd>
  <trd 
lng="el">&lambda;&alfa;&gamma;&omega;&ny;&jota;&kappa;&omikron_ton;</trd>
  <trd lng="fr">l&eacute;vrier</trd>
  <trd lng="hu">ag&aacute;r</trd>
  <trd lng="nl">hazewindhond</trd>
  <trd lng="pt">galgo</trd>
  <trd lng="ru">&c_g;&c_o;&c_n;&c_ch;&c_a;&c_ja; 
<klr>(&c_s;&c_o;&c_b;&c_a;&c_k;&c_a;)</klr></trd>
</drv>

<drv mrk="hund.mel0o">
  <kap>mel<tld/>o</kap>
  <snc mrk="hund.mel0o.ZOO">
    <dif>
      <ref tip="super" cel="hund.0o.dombesto"><tld lit="H"/>o</ref>
      el raso mallongkrura, aparte ta&ubreve;ganta por &ccirc;asi 
bestojn lo&gcirc;antajn en
      tertruoj.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_k;&c_s;&c_a; <klr 
tip="ind">(&c_s;&c_a;&c_b;&c_a;&c_k;&c_a;)</klr></trd>                            

  <trd lng="br">ki-douar</trd>
  <trd lng="cs">jezev&ccaron;&iacute;k</trd>
  <trd lng="de">Dackel</trd>
  <trd lng="en">dachshund</trd>
  <trdgrp lng="fr">
    <trd>basset</trd>,
    <trd>teckel</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tacsk&oacute;</trd>,
    <trd>dakszli</trd>,
    <trd>borzeb</trd>
  </trdgrp>
  <trd lng="nl">dashond</trd>
  <trd lng="pl">jamnik</trd>
  <trd lng="pt">bass&ecirc;</trd>
  <trd lng="ru">&c_t;&c_a;&c_k;&c_s;&c_a; <klr 
tip="amb">(&c_p;&c_o;&c_r;&c_o;&c_d;&c_a; 
&c_s;&c_o;&c_b;&c_a;&c_k;)</klr></trd>
</drv>

<drv mrk="hund.sxaf0o">
  <kap>&scirc;af<tld/>o</kap>
  <snc mrk="hund.sxaf0o.ZOO">
    <dif>
      <ref tip="super" cel="hund.0o.dombesto"><tld lit="H"/>o</ref> el
      grupo de
      rasoj uzataj origine de pa&scirc;tistoj por gardi brutaron kaj nun 
anka&ubreve;
      por diversaj gardaj kaj ser&ccirc;aj celoj. &Scirc;af<tld/>oj 
estas altaj kaj
      havas densan hararon.
    </dif>
  </snc>
  <trd 
lng="be">&c_a;&c_w;&c_ch;&c_a;&c_r;&c_k;&c_a;</trd>                            

  <trd lng="br">ki-de&ntilde;ved</trd>
  <trd lng="ca">gos d'<ind>atura</ind></trd>
  <trd lng="cs">ov&ccaron;&aacute;ck&yacute; pes</trd>
  <trd lng="de">Sch&auml;ferhund</trd>
  <trd lng="en">sheep dog</trd>
  <trd lng="fr">chien de berger</trd>
  <trd lng="hu">juh&aacute;szkutya</trd>
  <trd lng="nl">herdershond</trd>
  <trd lng="nl">herdershond</trd>
  <trd lng="pl">owczarek</trd>
  <trd lng="pt">c&atilde;o pastor</trd>
  <trd lng="ru">&c_o;&c_v;&c_ch;&c_a;&c_r;&c_k;&c_a;</trd>
</drv>
</art>
