<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>hund</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="hund.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="hund.0o.genro">
    <dif>
      <ref tip="lst" cel="best.rabo0o"
      lst="voko:rabobestoj">Rabobesto</ref>,
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="hunded.0oj"><tld/>edoj
      </ref> (<trd lng="la">Canis</trd>).
    </dif>
    <trd lng="tr">taz&inodot;</trd>
  </snc>
  <snc mrk="hund.0o.dombesto">
    <dif>
      Dombesto apartenanta al tiu genro, devenanta de lupo, kun multaj
      rasoj, uzata precipe por gardado kaj &ccirc;asado:
      <ekz>
        li promenas kun tri <tld/>oj
        <fnt><bib>F</bib>, <lok>&FE; 12</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>o estas plej bona amiko de homo;
      </ekz>
      <ekz>
        <tld/>o gardanta du pordojn restos malsata<fnt>Z</fnt>
        <klr>(oni ne povas servi du mastrojn samtempe)</klr>;
      </ekz>
      <ekz>
        bojas <tld/>o sen puno e&ccirc; kontra&ubreve; la suno
        <klr>(tion oni diras al iu, kiu miras, ke oni lin kritikas)
        </klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>oni lin konas kiel makulharan <tld/>on</ind>
        <klr>(li estas de &ccirc;iuj konata)</klr>
        <trd lng="fr">il est <ind>connu</ind> comme le loup blanc</trd>
        <fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>o bonrasa estas bona por &ccirc;aso
        <klr>(li havas la kvalitojn a&ubreve; la mankojn de siaj
        parencoj a&ubreve; avoj)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        mordantaj <tld/>oj kuras &ccirc;iam kun vundoj
        <klr>(malpaculon &ccirc;iu batas)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>bojanta <tld/>o ne mordas</ind>
        <klr>(kiu multe minacas, ne estas la plej dan&gcirc;era)</klr>
        <trd lng="fr">chien qui aboie ne mord pas</trd>
        <fnt>Z</fnt>;
      </ekz>
      <ekz>
        ne timu <tld/>on bojantan, timu <tld/>on silentan
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>por <tld/>on dronigi oni nomas &gcirc;in rabia</ind>
        <klr>(oni &ccirc;iam trovas pretekston por malutili malamaton)
        </klr>
        <trd lng="fr">pour noyer son chien on lui trouve la
        rage</trd>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="hund.dom0o">dom<tld/>o</ref>
  </snc>
  <trd lng="ar">&a_k;&a_fatha;&a_l;&a_sukun;&a_b;&a_dammatan; &a_j;
  &a_k;&a_kasra;&a_l;&a_fatha_A;&a_b;&a_dammatan;</trd>
  <trd lng="be">&c_s;&c_a;&c_b;&c_a;&c_k;&c_a;</trd>
  <trd lng="bg">&c_k;&c_u;&c_ch;&c_je;</trd>
  <trd lng="br">ki</trd>
  <trdgrp lng="ca">
    <trd>gos</trd>,
    <trd>ca</trd>
  </trdgrp>
  <trd lng="cs">pes</trd>
  <trd lng="de">Hund</trd>
  <trd
  lng="el">&sigma;&kappa;&ypsilon_ton;&lambda;&omikron;&sigma_fina;</trd>
  <trd lng="en">dog</trd>
  <trd lng="es">perro</trd>
  <trd lng="fa">&a_s;&f_g;</trd>
  <trd lng="fr">chien</trd>
  <trd lng="hu">kutya</trd>
  <trd lng="id">anjing</trd>
  <trd lng="it">cane</trd>
  <trd lng="nl">hond</trd>
  <trd lng="oc">can</trd>
  <trd lng="pl">pies</trd>
  <trdgrp lng="pt">
    <trd>cachorro</trd>,
    <trd>c&atilde;o</trd>
  </trdgrp>
  <trd lng="ro">c&acirc;ine</trd>
  <trd lng="sk">pes</trd>
  <trd lng="sv">hund</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_b;&c_a;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_jo;&c_s;</trd>
  </trdgrp>
  <trd lng="tr">k&ouml;pek</trd>
  <trd lng="vo">dog</trd>
</drv>

<drv mrk="hund.0a">
  <kap><tld/>a</kap>
  <snc mrk="hund.0a.ZOO">
    <dif>
      De <tld/>o:
      <ekz>
        <tld/>a mordo.
      </ekz>
    </dif>
    <trd lng="ca">can&iacute;</trd>
    <trd lng="cs">ps&iacute;</trd>
    <trd lng="en">canine</trd>
    <trd lng="hu">kutya-</trd>
    <trd lng="it">canino<klr> (agg.)</klr></trd>
  </snc>
  <snc mrk="hund.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Mizera; konvena por besto, ne por homo:
      <ekz>
        <ind><tld/>a vivo</ind>
        <trdgrp lng="fa">
          <trd>&a_z;&a_n;&a_d;&f_g;&f_y; &a_s;&f_g;&f_y;</trd>,
          <trd>&a_z;&a_n;&a_d;&f_g;&f_y;
          &a_n;&f_k;&a_b;&a_t;&zwnj;&a_b;&a_A;&a_r;</trd>
        </trdgrp>
        <trd lng="fr">une <ind>vie</ind> de chien</trd>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="pl">pieskie &zdot;ycie</trd>
        <trd lng="ru">&c_s;&c_o;&c_b;&c_a;&c_ch;&c_mol;&c_ja;
        &c_zh;&c_i;&c_z;&c_n;&c_mol;</trd>
      </ekz>
    </dif>
    <trd lng="hu">kutya<klr tip="ind"> (&aacute;tv.)</klr></trd>
    <trd lng="it">da <ind>cane</ind></trd>
    <trd lng="pl">pieski</trd>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_b;&c_a;&c_ch;&c_y;</trd>
  <trd lng="br">-<ind>ki</ind></trd>
  <trd
  lng="el">&sigma;&kappa;&ypsilon;&lambda;&jota_ton;&sigma;&jota;&omikron;&sigma_fina;</trd>
  <trdgrp lng="es">
    <trd>perro</trd>,
    <trd>perra</trd>,
    <trd>perruno</trd>,
    <trd>perruna</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd><klr tip="amb">(&a_m;&a_r;&a_b;&a_w;&a_T; &a_b;&a_h;)
    </klr>&a_s;&f_g;</trd>,
    <trd>&a_s;&f_g;&f_y;</trd>,
    <trd>&a_s;&f_g;&zwnj;&a_s;&a_A;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>canin</trd>,
    <trd>de <ind>chien</ind></trd>
  </trdgrp>
  <trd lng="nl">honds</trd>
  <trd lng="pl">psi</trd>
  <trd lng="pt">canino</trd>
  <trd lng="ru">&c_s;&c_o;&c_b;&c_a;&c_ch;&c_i;&c_j;</trd>
  <trd lng="sk">ps&iacute;</trd>
</drv>

<drv mrk="hund.cxas0o">
  <kap>&ccirc;as<tld/>o</kap>
  <snc mrk="hund.cxas0o.ZOO">
    <dif>
      <ref tip="super" cel="hund.0o.dombesto"><tld lit="H"/>o</ref>
      uzata en &ccirc;aso por trovo, persekuto, mortigo a&ubreve;
      alporto de bestoj:
      <ekz>
        li balda&ubreve; renkontis ku&scirc;antan &ccirc;as<tld/>on, kiu
        spiregis kiel iu, kiu laci&gcirc;is de la kurado
        <fnt><bib>ElektFab</bib>, <lok>la bremenaj muzikistoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ja;&c_w;&c_n;&c_ib;&c_ch;&c_y;
  &c_s;&c_a;&c_b;&c_a;&c_k;&c_a;</trd>
  <trdgrp lng="br">
    <trd>ki-chase</trd>,
    <trd>ki-hemolc'h</trd>
  </trdgrp>
  <trd lng="ca">gos de ca&ccedil;a</trd>
  <trd lng="de">Jagdhund</trd>
  <trdgrp lng="en">
    <trd>hunting dog</trd>,
    <trd>hunting hound</trd>
  </trdgrp>
  <trd lng="es">perro de caza</trd>
  <trd lng="fa">&a_s;&f_g; &a_sh;&f_k;&a_A;&a_r;&f_y;</trd>
  <trd lng="fr">chien de chasse</trd>
  <trd lng="hu">vad&aacute;szkutya</trd>
  <trd lng="id"><ind>anjing</ind> pemburu</trd>
  <trd lng="it"><ind>cane</ind> da caccia</trd>
  <trd lng="nl">jachthond</trd>
  <trdgrp lng="pl">
    <trd>pies go&nacute;czy</trd>,
    <trd>pies <ind>my&sacute;liwski</ind></trd>
  </trdgrp>
  <trd lng="pt">c&atilde;o de ca&ccedil;a</trd>
  <trd lng="ro">c&acirc;ine de v&acirc;n&abreve;toare</trd>
  <trd lng="ru">&c_o;&c_h;&c_o;&c_t;&c_n;&c_i;&c_ch;&c_mol;&c_ja;
  &c_s;&c_o;&c_b;&c_a;&c_k;&c_a;</trd>
  <trd lng="sv">jakthund</trd>
  <trdgrp lng="tr">
    <trd>av k&ouml;pe&gbreve;i</trd>
  </trdgrp>
</drv>

<drv mrk="hund.Cxas0oj">
  <kap>&Ccirc;as<tld/>oj</kap>
  <mlg>CVn</mlg>
  <snc mrk="hund.Cxas0oj.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Nord-hemisfera stelfiguro (<trd lng="la">Canes Venatici</trd>).
    </dif>
  </snc>
  <trd lng="ca">constel&middot;laci&oacute; dels Llebrers</trd>
  <trd lng="de">Jagdhunde</trd>
  <trd lng="en">Canes Venatici</trd>
  <trdgrp lng="es">
    <trd>Los <ind>Perros</ind> de Caza</trd>,
    <trd>Lebreles</trd>
  </trdgrp>
  <trd lng="fa">&a_t;&a_A;&a_z;&f_y;&a_A;&a_n;<klr tip="ind">
  (&a_S;&a_w;&a_r;&a_t;
  &a_f;&a_l;&f_k;&f_y;)</klr></trd>
  <trdgrp lng="it">
   <trd><ind>Cani</ind> da caccia</trd>, 
   <trd>Levrieri</trd>
  </trdgrp>
  <trd lng="pl">Psy Go&nacute;cze</trd>
  <trd lng="ro">C&acirc;inii de V&acirc;n&abreve;toare</trd>
  <trd lng="ru">
  &c_G;&c_o;&c_n;&c_ch;&c_i;&c_je; &c_P;&c_s;&c_y;<klr tip="amb">
  (&c_s;&c_o;&c_z;&c_v;&c_je;&c_z;&c_d;&c_i;&c_je;)</klr></trd>
</drv>

<drv mrk="hund.cxen0o">
  <kap>&ccirc;en<tld/>o</kap>
  <snc mrk="hund.cxen0o.ZOO">
    <dif>
      <ref tip="super" cel="hund.0o.dombesto"><tld lit="H"/>o</ref> kiu
      gardas domon a&ubreve; pasejon, tenate per &ccirc;eno:
      <ekz>
        krom tio trovi&gcirc;as anka&ubreve; &ccirc;en<tld/>o, kiu bojas
        kontra&ubreve; &ccirc;io kaj &ccirc;iu
        <fnt><bib>Fab4</bib>, <lok>kion la patro faras, estas &ccirc;iam
        &gcirc;usta</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">ki-porzh</trd>
  <trdgrp lng="de">
    <trd>Kettenhund</trd>,
    <trd>Wachhund</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_s;&f_g; &a_n;&f_g;&a_h;&a_b;&a_A;&a_n;</trd>,
    <trd>&a_s;&f_g; &f_p;&a_A;&a_s;&a_b;&a_A;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">chien de garde<klr tip="amb"> (&agrave; l'attache)</klr>
  </trd>
  <trd lng="it"><ind>cane</ind> da guardia</trd>
  <trd lng="pl">pies podw&oacute;rzowy</trd>
  <trd lng="pt">c&atilde;o de guarda</trd>
  <trdgrp lng="ru">
    <trd>&c_c;&c_je;&c_p;&c_n;&c_a;&c_ja; &c_s;&c_o;&c_b;&c_a;&c_k;&c_a;
    </trd>,
    <trd>&c_c;&c_je;&c_p;&c_n;&c_o;&c_j; &c_p;&c_jo;&c_s;</trd>
  </trdgrp>
</drv>

<drv mrk="hund.0ejo">
  <kap><tld/>ejo</kap>
    <snc mrk="hund.0ejo.KOMUNE">
      <dif>
      Loko speciale aran&gcirc;ita por ke vivu eksterdome kaj
      malliberece la
      hundoj:
     <ekz>
       &ccirc;i-nokte mi sidis sur la &scirc;tiparo apud la <tld/>ejo
       kaj amuzis
       min inciteti la alligitan <tld/>on
       <fnt>
         <aut>H. C. Andersen, trad. K. Knopf</aut>,
         <vrk><url
         ref="http://donh.best.vwh.net/Esperanto/Literaturo/Noveloj/Fabeloj_de_Andersen/KoboldetoH.html">
         Koboldeto kaj sinjorino</url></vrk>,
       </fnt>.
     </ekz> 
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>gossera</trd>,
    <trd>canera</trd>
  </trdgrp>
  <trd lng="cs">ps&iacute; bouda</trd>
  <trd lng="en">kennel</trd>
  <trd lng="es">caseta del perro</trd>
  <trd lng="fa">&a_l;&a_A;&a_n;&a_h;&a_hamza_sure;
  <ind>&a_s;&f_g;</ind></trd>
  <trd lng="fr">chenil</trd>
  <trd lng="id"><ind>kandang</ind> anjing</trd>
  <trd lng="it">canile</trd>
  <trd lng="sk">psia b&uacute;da</trd>
  <trd lng="tr">k&ouml;pek kul&uuml;besi</trd>
</drv>

<drv mrk="hund.dom0o">
  <kap>dom<tld/>o</kap>
  <snc mrk="hund.dom0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="hund.0o.dombesto"><tld lit="H"/>o
    <sncref ref="hund.0o.dombesto"/></ref>
    <ref tip="super" cel="hund.0o.genro">hundo <sncref
    ref="hund.0o.genro"/></ref>
  </snc>
  <trdgrp lng="id">
    <trd><ind>anjing</ind> rumah</trd>,
    <trd><ind>anjing</ind> peliharaan</trd>
  </trdgrp>
  <trd lng="it"><ind>cane</ind> domestico</trd>
  <trd lng="ca">gos <ind>dom&egrave;stic</ind></trd>
</drv>

<drv mrk="hund.gard0o">
  <kap>gard<tld/>o</kap>
  <snc mrk="hund.gard0o.HEJM">
    <dif>
      <tld lit="H"/>o kiu gardas domon, alarmas kaj defendas 
      kontra&ubreve; fremduloj:
      <ekz>
        niaj gard<tld/>oj &ccirc;irka&ubreve;is &scirc;in kaj aljelpis
        &scirc;in
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">hl&iacute;dac&iacute; pes</trd>
  <trd lng="es">perro guardi&aacute;n</trd>
  <trd lng="fr">chien de garde</trd>
  <trd lng="sk">str&aacute;&zcaron;ny pes</trd>
</drv>

<drv mrk="hund.lepor0o">
  <kap>lepor<tld/>o</kap>
  <snc mrk="hund.lepor0o.ZOO">
    <dif>
      <ref tip="super" cel="hund.0o.dombesto"><tld lit="H"/>o</ref> el
      raso rapidkura, gracie maldika, kapabla kurkapti leporon.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_n;&c_ch;&c_a;&c_k;</trd>
  <trd lng="br">levran</trd>
  <trd lng="ca">llebrer</trd>
  <trdgrp lng="cs">
    <trd>chrt</trd>,
    <trd>loveck&yacute; pes</trd>,
    <trd>oha&rcaron;</trd>
  </trdgrp>
  <trd lng="de">Windhund</trd>
  <trd
  lng="el">&lambda;&alfa;&gamma;&omega;&ny;&jota;&kappa;&omikron_ton;</trd>
  <trdgrp lng="en">
    <trd>harehound</trd>,
    <trd>harrier <klr tip="amb">(dog)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>lebrel</trd>,
    <trd>galgo</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_t;&a_A;&a_z;&f_y;</trd>,
    <trd>&a_s;&f_g; &a_t;&a_A;&a_z;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">l&eacute;vrier</trd>
  <trd lng="hu">ag&aacute;r</trd>
  <trd lng="it">levriero</trd>
  <trd lng="nl">hazewindhond</trd>
  <trd lng="pl">chart</trd>
  <trd lng="pt">galgo</trd>
  <trd lng="ro">ogar</trd>
  <trd lng="ru">&c_g;&c_o;&c_n;&c_ch;&c_a;&c_ja; <klr
  tip="amb">(&c_s;&c_o;&c_b;&c_a;&c_k;&c_a;)</klr></trd>
  <trd lng="sk">chrt</trd>
</drv>

<drv mrk="hund.mel0o">
  <kap>mel<tld/>o</kap>
  <snc mrk="hund.mel0o.ZOO">
    <dif>
      <ref tip="super" cel="hund.0o.dombesto"><tld lit="H"/>o</ref>
      el raso mallongkrura, aparte ta&ubreve;ga por &ccirc;asi bestojn
      lo&gcirc;antajn en tertruoj.
    </dif>
    <ref tip="vid" cel="vertag.0o">vertago</ref>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_k;&c_s;&c_a; <klr tip="ind">
  (&c_s;&c_a;&c_b;&c_a;&c_k;&c_a;)</klr></trd>
  <trd lng="bg">&c_d;&c_a;&c_k;&c_je;&c_l;</trd>
  <trd lng="br">ki-douar</trd>
  <trd lng="ca">gos salsitxa</trd>
  <trd lng="cs">jezev&ccaron;&iacute;k</trd>
  <trd lng="de">Dackel</trd>
  <trd lng="en">dachshund</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&f_g; &f_p;&a_A;&f_k;&a_w;&a_t;&a_A;&a_h;</trd>,
    <trd>&a_d;&a_A;&f_k;&a_s;&a_w;&a_n;&a_d;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>basset</trd>,
    <trd>teckel</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tacsk&oacute;</trd>,
    <trd>dakszli</trd>,
    <trd>borzeb</trd>
  </trdgrp>
  <trd lng="id">dachshund</trd>
  <trd lng="it">bassotto<klr> (cane)</klr></trd>
  <trd lng="nl">dashond</trd>
  <trd lng="pl">jamnik</trd>
  <trd lng="pt">bass&ecirc;</trd>
  <trd lng="ru">&c_t;&c_a;&c_k;&c_s;&c_a; <klr tip="amb">
  (&c_p;&c_o;&c_r;&c_o;&c_d;&c_a; &c_s;&c_o;&c_b;&c_a;&c_k;)</klr></trd>
  <trd lng="sk">jazve&ccaron;&iacute;k</trd>
</drv>

<drv mrk="hund.sxaf0o">
  <kap>&scirc;af<tld/>o</kap>
  <snc mrk="hund.sxaf0o.ZOO">
    <dif>
      <ref tip="super" cel="hund.0o.dombesto"><tld lit="H"/>o</ref> el
      grupo de rasoj uzataj origine de pa&scirc;tistoj por gardi
      brutaron kaj nun anka&ubreve; por diversaj gardaj kaj
