<drv mrk="hund.0a">
  <kap><tld/>a</kap>
  <snc mrk="hund.0a.ZOO">
    <dif>
      De <tld/>o, rilata al <tld/>o:
      <ekz>
        <tld/>a mordo;
      </ekz><ekz>
        mi havas <tld/>an flarsenton
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">can&iacute;</trd>
    <trd lng="cs">ps&iacute;</trd>
    <trd lng="en">canine</trd>
    <trd lng="hu">kutya-</trd>
    <trd lng="it">canino<klr> (agg.)</klr></trd>
  </snc>
  <snc mrk="hund.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Mizera; konvena por besto, ne por homo:
      <ekz>
        <ind><tld/>a vivo</ind>
        <trdgrp lng="fa">
          <trd>&a_z;&a_n;&a_d;&f_g;&f_y; &a_s;&f_g;&f_y;</trd>,
          <trd>&a_z;&a_n;&a_d;&f_g;&f_y;
          &a_n;&f_k;&a_b;&a_t;&#8204;&a_b;&a_A;&a_r;</trd>
        </trdgrp>
        <trd lng="fr">une <ind>vie</ind> de chien</trd>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="pl">pieskie &zdot;ycie</trd>
        <trd lng="ro">via&tcedil;&abreve; de  <ind>c&acirc;ine</ind></trd>
        <trd lng="ru">&c_s;&c_o;&c_b;&c_a;&c_ch;&c_mol;&c_ja;
        &c_zh;&c_i;&c_z;&c_n;&c_mol;</trd>
      </ekz>
    </dif>
    <trd lng="hu">kutya<klr tip="ind"> (&aacute;tv.)</klr></trd>
    <trd lng="it">da <ind>cane</ind></trd>
    <trd lng="pl">pieski</trd>
    <trd lng="ro">c&acirc;inelui </trd>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_b;&c_a;&c_ch;&c_y;</trd>
  <trd lng="br">-<ind>ki</ind></trd>
  <trd lng="el">&sigma;&kappa;&ypsilon;&lambda;&jota_ton;&sigma;&jota;&omikron;&sigma_fina;</trd>
  <trdgrp lng="es">
    <trd>perro</trd>,
    <trd>perra</trd>,
    <trd>perruno</trd>,
    <trd>perruna</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd><klr tip="amb">(&a_m;&a_r;&a_b;&a_w;&a_T; &a_b;&a_h;)
    </klr>&a_s;&f_g;</trd>,
    <trd>&a_s;&f_g;&f_y;</trd>,
    <trd>&a_s;&f_g;&#8204;&a_s;&a_A;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>canin</trd>,
    <trd>de <ind>chien</ind></trd>
  </trdgrp>
  <trd lng="io">hundala</trd>
  <trd lng="nl">honds</trd>
  <trd lng="pl">psi</trd>
  <trd lng="pt">canino</trd>
  <trd lng="ro">canin</trd>
  <trd lng="ru">&c_s;&c_o;&c_b;&c_a;&c_ch;&c_i;&c_j;</trd>
  <trd lng="sk">ps&iacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_o;&c_b;&c_a;&c_ch;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_je;&c_s;&c_ja;&c_ch;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_s;&c_ja;&c_ch;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="vo">dogik</trd>
</drv>

