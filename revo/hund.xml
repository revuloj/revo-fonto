<drv mrk="hund.0aro">
  <kap>
    <tld/>aro
  </kap>
  <snc>
    <dif>
      Grupo de <tld/>oj, komune &ccirc;asanta, tiranta glitveturilon ks:
      <ekz>
        anstata&ubreve; &ccirc;asadi dekkornan cervon, apron la <tld/>aro persekutis
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        sonas trumpetoj, oni ellasas <tld/>aron
        <fnt>
          <bib>SrT</bib>
          <lok>Libro II.</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Meute</trd>
  <trd lng="en">pack <klr>(of dogs)</klr></trd>
  <trd lng="fr">meute</trd>
  <trd lng="ru">&c_sh;&c_a;&c_j;&c_k;&c_a;</trd>
</drv>

<drv mrk="hund.0ido">
  <kap><tld/>ido</kap>
  <snc>
    <dif>
      <ref tip="super" cel="id.0o.BIO">Ido<sncref/></ref> de <tld/>o:
      <ekz>
        mi prenas suba&ccirc;etojn, sed <klr>[&#x2026;]</klr> en formo de &ccirc;asaj <tld/>idoj
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la buboj de la korto kuris <klr>[&#x2026;]</klr> kiel <tld/>idoj
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>X</lok>
        </fnt>;
      </ekz>
      <ekz>
        bojas <tld/>ido, &ccirc;ar tiel faras la <tld/>o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_ch;&c_a;&c_n;&c_ja;</trd>,
    <trd>&c_sh;&c_ch;&c_a;&c_n;&c_jo;</trd>,
    <trd>&c_sh;&c_ch;&c_a;&c_n;&c_ju;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Welpe</trd>
  <trd lng="fr">chiot</trd>
  <trd lng="hu">kutyak&ouml;ly&ouml;k</trd>
  <trd lng="id">anak anjing</trd>
  <trd lng="ja">&#x5B50;&#x72AC;  <pr>&#x3053;&#x3044;&#x306C;</pr></trd>
  <trdgrp lng="nl">
    <trd>pup</trd>,
    <trd>puppy</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>szczeni&eogonek;</trd>,
    <trd>szczeniak</trd>
  </trdgrp>
  <trd lng="ro">c&abreve;&tcomma;el</trd>
  <trd lng="ru">&c_shch;&c_je;&c_n;&c_o;&c_k;</trd>
  <trd lng="sv">valp</trd>
  <trd lng="zh">&#x5C0F;&#x72D7; <pr>xi&#x1CE;og&#x1D2;u</pr></trd>
</drv>

<drv mrk="hund.0ino">
  <kap><tld/>ino</kap>
  <snc>
    <dif>
      Inseksa <tld/>o:
      <ekz>
        <klr>[se]</klr> servisto piedbatus <tld/>inon, kiu nutras la idojn, mi forpelus
        lin
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        li flaras &scirc;ian parfumon adore, kvaza&ubreve; <tld/>o <tld/>inon
        <fnt>
          <bib>CKv</bib>
          <lok>7</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &leftquot;vi trudema <tld/>ino!&rightquot; &ndash; li levis sian glavon
        <fnt>
          <bib>MJA</bib>
          <lok>Parto Oka</lok>
        </fnt>.
      </ekz>
