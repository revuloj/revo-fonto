<art mrk="$Id$">
<kap>
<rad>e&ubreve;forbi</rad>/o
</kap>
<drv mrk="euxfor.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="euxfor1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Euphorbia</trd>),
      kun blanka venena suko, kaj kies radikoj efikas laksige.
