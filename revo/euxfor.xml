<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>e&ubreve;forbi</rad>/o
</kap>
<drv mrk="euxfor.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="euxfor.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Euphorbia</trd>),
      kun blanka venena suko, kaj kies radikoj efikas laksige.
    </dif>
  </snc>
  <bld lok="../bld/euxfor1.jpg">
    krucfolia <tld/>o (<trd lng="la">Euphorbia lathyris</trd>)
  </bld>
  <bld lok="../bld/euxfor2.jpg">
    <trd lng="la">Euphorbia milii</trd>
  </bld>
  <trd lng="de">Wolfsmilch</trd>
</drv>
<drv mrk="euxfor.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj unu- a&ubreve; senpetalaj
      vegeta&jcirc;oj (<trd lng="la">Euphorbiaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Wolfsmilchgew&auml;chse</trd>
</drv>
</art>
