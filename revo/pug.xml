<vortaro>
<art mrk="$Id$">
<kap>
  <rad>pug</rad>/o
</kap>
<drv mrk="pug.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      La du <ref tip="prt" cel="glute.0o">gluteoj</ref> &ccirc;e homo:
      <ekz>
        la bel<tld/>a Afrodita <klr>(fama helena statuo)</klr>;
      </ekz>
      <ekz>
        <tld/>mezuro <klr>(por al&gcirc;ustigo de jupo a&ubreve; de
pantalono)</klr>.
      </ekz>
    </dif>
    <ref tip="sin" cel="post.0ajxo.pugo">posta&jcirc;o</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Hintern</trd>,
    <trd>Arsch</trd>,
    <trd>Popo</trd>,
    <trd>Po</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>buttocks</trd>,
    <trd>rump</trd>,
    <trd>backside</trd>,
    <trd>cheeks</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>trasero</trd>,
    <trd>nalgas</trd>,
    <trd>culo</trd>
  </trdgrp>
  <trd lng="fr">cul</trd>
  <trdgrp lng="hu">
    <trd>far</trd>,
    <trd>h&aacute;ts&oacute;</trd>,
    <trd>fen&eacute;k</trd>
  </trdgrp>
  <trd lng="nl">achterwerk</trd>
  <trd lng="ru">&c_z;&c_a;&c_d;&c_n;&c_i;&c_c;&c_a;</trd>
</drv>
</art>
