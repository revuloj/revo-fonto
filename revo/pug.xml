<drv mrk="pug.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      La du <ref tip="prt" cel="vang.sid0o">sidvangoj</ref> &ccirc;e homo:
      <ekz>
        la <ind>bel<tld/>a</ind> Afrodita <klr>(fama helena
        statuo)</klr>;
        <trd lng="de">kallipygisch</trd>
        <trd lng="el">&kappa;&alfa;&lambda;&lambda;&jota_ton;&pi;&ypsilon;&gamma;&omikron;&sigma_fina;</trd>
        <trd lng="en">callipygian</trd>
        <trd lng="es">calipigio</trd>
        <trd lng="fr">callipyge</trd>
        <trd lng="it">callipige</trd>
        <trd lng="pt">calip&iacute;gio</trd>
      </ekz>
      <ekz>
        <ind><tld/>mezuro</ind> <klr>(por al&gcirc;ustigo de jupo a&ubreve;
        de pantalono)</klr>;
        <trd lng="fr">tour de hanches</trd>
      </ekz>
      <ekz>
        unu supra&jcirc;a rigardo sur lian viza&gcirc;on sufi&ccirc;as
        por kompreni, ke li estas malnobla, &ccirc;ikanema, senprincipa,
        <ind><tld/>olekema</ind> kanajlo <klr>(flatemulo)</klr>
        <fnt><bib>MkM</bib><lok>&ccirc;ap. 7</lok></fnt>.
        <trd lng="fr">l&egrave;che-cul<klr tip="ind"> (adj.)</klr></trd>
      </ekz>
    </dif>
    <ref tip="vid" cel="post.0ajxo.pugo">posta&jcirc;o</ref>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_d;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="bg">&c_d;&c_u;&c_p;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>boky</trd>,
    <trd>zadek</trd>,
    <trd>zadnice</trd>,
    <trd>zv&iacute;&rcaron;ec&iacute; &rcaron;i&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hintern</trd>,
    <trd>Arsch</trd>,
    <trd>Popo</trd>,
    <trd>Po</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>buttocks</trd>,
    <trd>rump</trd>,
    <trd>backside</trd>,
    <trd>cheeks</trd>,
    <trd>butt</trd>,
    <trd>ass</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>trasero</trd>,
    <trd>nalgas</trd>,
    <trd>culo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>derri&egrave;re<klr tip="ind"> (ana.)</klr></trd>,
    <trd>post&eacute;rieur<klr tip="ind"> (ana.)</klr></trd>,
    <trd>cul</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>far</trd>,
    <trd>h&aacute;ts&oacute;</trd>,
    <trd>fen&eacute;k</trd>
  </trdgrp>
  <trd lng="io">kulo</trd>
  <trdgrp lng="it">
    <trd>sedere</trd>,
    <trd>culo</trd>,
    <trd>didietro</trd>
