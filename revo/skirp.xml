<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: skirp.xml,v 1.3 2022/05/05 05:09:57 afido Exp $">
    <kap>
      <rad>skirp</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="skirp.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref> 
      el familio <ref tip="malprt" cel="ciper.0oj">ciperacoj</ref> 
      (<trd lng="la">Scirpus</trd>), kies specioj
      preferas malsekan grundon;
      ordinare kun trie&gcirc;aj tigoj kaj longaj mallar&gcirc;aj folioj en tri vicoj,
      brunaj spiketoj el malgrandaj, ne okulfrapaj floroj:
      <ekz>
        se la strando ne estas sabla, oni fal&ccirc;as kaj elradikigas <tld/>ojn kaj
        fragmitojn, forlevas &scirc;tonojn de la fundo kaj eble alportigas sablon
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Steloj;monato2/007162.html"
              >&Gcirc;ojoj kaj &gcirc;uoj de finna somero</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la nesto <klr>[de makulanaso]</klr> estas konstru<klr>[a]</klr>ta surgrunde
        inter vegeta&jcirc;aro, kiaj <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=386182"
              >Makulanaso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="junk.0o">junko</ref>,
<ref cel="fragmi.0o">fragmito</ref>
    </refgrp>
    <bld
      lok="&WCU;/thumb/c/c3/Scirpus_sylvaticus_RF.jpg/383px-Scirpus_sylvaticus_RF.jpg"
      prm="CC BY 4.0"
      ><tld/>o (Scirpus sylvaticus) &ccirc;e Dreieich-G&ouml;tzenhain, Germanujo
        <fnt>
          <aut>Robert Flogaus-Faust, propra verko</aut>
          <lok><url ref="&WCW;/File:Scirpus_sylvaticus_RF.jpg"
              >&WCW;/File:Scirpus_sylvaticus_RF.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Simse</trd>
    <trd lng="en">scirpus</trd>
    <trd lng="fr">scirpus</trd>
    <trd lng="ru">&c_k;&c_a;&c_m;&c_y;&c_sh;</trd>
    <trd lng="uk">&c_k;&c_o;&c_m;&c_i;&c_sh;</trd>
  </snc>
</drv>

  </art>

