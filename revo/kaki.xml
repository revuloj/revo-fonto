<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kaki.xml,v 1.2 2018/11/16 23:10:16 revo Exp $">
<kap>
  <rad>kaki</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="kaki.0a">
  <kap><tld/>a</kap>
  <snc mrk="kaki.0a.KOL">
    <dif>
      Brune verda:
      <ekz>
        alvenis la polica le&ubreve;tenanto, junulo tre bela kaj vireca
        en sia 
        <tld/>a uniformo
        <fnt>
          <aut>L. N. Newell</aut>,
          <vrk><url
          ref="http://literaturo.org/HARLOW-Don/Esperanto/Literaturo/Noveloj/baksis/piulo.html">
          La &Jcirc;uro de Piulo</url></vrk>,
          <lok>en: Bak&scirc;i&scirc;, 1938</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">kaki<klr> (adj. couleur)</klr></trd>
  <trd lng="pl">khaki <klr>(w kolorze)</klr></trd>
</drv>


<drv mrk="kaki.0o">
  <kap><tld/>o</kap>
  <snc mrk="kaki.0o.KOL">
    <dif>
      Brune verda <ref tip="lst" cel="kolor.0o.propre"
      lst="voko:koloroj">koloro</ref>:
      <ekz>
        la makulita <tld/>o de la &ccirc;emizoj
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">kaki<klr> (subst. couleur)</klr></trd>
  <trd lng="pl">khaki</trd>     
  <trd lng="ro">kaki</trd>
</drv>
</art>

