<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kaki.xml,v 1.9 2023/12/02 21:20:04 revo Exp $">
<kap>
  <rad>kaki</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="kaki.0a">
  <kap><tld/>a</kap>
  <snc mrk="kaki.0a.KOL">
    <dif>
      Brune verda:
      <ekz>
        alvenis la polica le&ubreve;tenanto, junulo tre bela kaj vireca
        en sia
        <tld/>a uniformo
        <fnt>
          <aut>L. N. Newell</aut>,
          <vrk><url ref="&DonH;Noveloj/baksis/piulo.html">
          La &Jcirc;uro de Piulo</url></vrk>,
          <lok>en: Bak&scirc;i&scirc;, 1938</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_l;&c_je;&c_r;&c_u; <ind>&c_h;&c_a;&c_k;&c_ib;</ind></trd>
  <trd lng="en">khaki</trd>
  <trd lng="fr">kaki<klr> (adj. couleur)</klr></trd>
  <trd lng="ja">&#x30AB;&#x30FC;&#x30AD;&#x8272;&#x306E;  <pr>&#x304B;&#x30FC;&#x304D;&#x3044;&#x308D;&#x306E;</pr></trd>
  <trd lng="pl">khaki <klr>(w kolorze)</klr></trd>
  <trd lng="uk">&c_k;&c_o;&c_l;&c_mol;&c_o;&c_r;&c_u; &c_h;&c_a;&c_k;&c_ib;</trd>
  <trdgrp lng="zh">
    <trd>&#x5361;&#x5176; <pr>k&#x1CE;q&#xED;</pr></trd>,
    <trd>&#x5494;&#x53FD; <pr>k&#x1CE;j&#x12B;</pr></trd>,
    <trd>&#x56FD;&#x9632;&#x8272; <pr>gu&#xF3;f&#xE1;ngs&#xE8;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kaki.0o">
  <kap><tld/>o</kap>
  <snc mrk="kaki.0o.KOL">
    <dif>
      Brune verda <ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">koloro</ref>:
      <ekz>
        la makulita <tld/>o de la &ccirc;emizoj
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_l;&c_je;&c_r; <ind>&c_h;&c_a;&c_k;&c_ib;</ind></trd>
  <trd lng="en">khaki</trd>
  <trd lng="fr">kaki<klr> (subst. couleur)</klr></trd>
  <trd lng="ja">&#x30AB;&#x30FC;&#x30AD;&#x8272;  <pr>&#x30AB;&#x30FC;&#x30AD;&#x3044;&#x308D;</pr></trd>
  <trd lng="pl">khaki</trd>
  <trd lng="ro">kaki</trd>
  <trd lng="uk">&c_h;&c_a;&c_k;&c_ib; (&c_ib;&c_m;.)</trd>
  <trdgrp lng="zh">
    <trd>&#x5361;&#x5176; <pr>k&#x1CE;q&#xED;</pr></trd>,
    <trd>&#x5494;&#x53FD; <pr>k&#x1CE;j&#x12B;</pr></trd>,
    <trd>&#x56FD;&#x9632;&#x8272; <pr>gu&#xF3;f&#xE1;ngs&#xE8;</pr></trd>
  </trdgrp>
</drv>
</art>

