<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxusx.xml,v 1.17 2022/03/29 14:20:29 revo Exp $">
<kap>
    <rad>&scirc;u&scirc;</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="sxusx.0i">
    <kap><tld/>i</kap>
    <gra><vspec>ntr</vspec></gra>
    <snc>
      <dif>
        <ref tip="super" cel="bru.0i.soni">Brui</ref> frote, simile al
        tirata &scirc;-sono:
        <ekz>
          oleo <tld/>as sur la pato;
        </ekz>
        <ekz>
          varmega fero <tld/>as en la akvo;
        </ekz>
        <ekz>
          La mineral'akvo <tld/>anta en la gor&gcirc;o!
         <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 5a, p. 48a</lok></fnt>.
        </ekz>
      </dif>
    </snc>
    <ref tip="vid" cel="sibl.0i">sibli</ref>
    <trdgrp lng="be">
      <trd>&c_sh;&c_a;&c_s;&c_t;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_sh;&c_a;&c_m;&c_a;&c_c;&c_je;&c_c;&c_mol;</trd>,
      <trd>&c_sh;&c_a;&c_p;&c_a;&c_c;&c_je;&c_c;&c_mol;</trd>,
      <trd>&c_sh;&c_a;&c_l;&c_ja;&c_s;&c_c;&c_je;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">zischen</trd>
    <trdgrp lng="fr">
      <trd>bruire</trd>,
      <trd>bruisser</trd>,
      <trd>chuinter</trd>
    </trdgrp>
    <trd lng="hu">sistereg</trd>
   <trdgrp lng="it">
      <trd>fischiare</trd>,
      <trd>sibilare</trd>,
      <trd>sfrigolare</trd>,
      <trd>soffiare<klr> (rif. al rumore o verso del gatto)</klr></trd>
    </trdgrp>
    <trd lng="nl">sissen</trd>
    <trdgrp lng="ru">
      <trd>&c_sh;&c_i;&c_p;&c_je;&c_t;&c_mol;</trd>,
      <trd>&c_sh;&c_u;&c_r;&c_sh;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_sh;&c_je;&c_l;&c_je;&c_s;&c_t;&c_je;&c_t;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>brusa</trd>,
      <trd>fr&auml;sa<klr> (ljud)</klr></trd>
    </trdgrp>
  <trd lng="uk">&#x448;&#x438;&#x43F;&#x456;&#x442;&#x438; (&#x43F;&#x440;&#x43E; &#x433;&#x430;&#x437; &#x456; &#x442;.&#x43F;.)</trd>
</drv>

<drv mrk="sxusx.0a_konsonanto">
    <kap><tld/>a konsonanto</kap>
    <uzo tip="fak">FON</uzo>
    <snc>
      <dif>
        <ref tip="super" cel="frot.0sono.FON">Frota konsonanto</ref>
        el la serio <ctl>&scirc;</ctl>, <ctl>&jcirc;</ctl>:
        <ekz>
          ili diris l anstata&ubreve; r, kaj en la <tld/>aj konsonantoj 
          ili iom lispis
          <fnt>
            <aut>Karel &Ccaron;apek, tr. Josef Vondrou&scaron;ek</aut>:
            <vrk>Milito kontra&ubreve; salamandroj</vrk>.
            <lok>INKO, 2003. Libro 2&#xAA;, &ccirc;ap. 3&#xAA;.</lok>
          </fnt>.
        </ekz>
      </dif>
    </snc>
    <ref tip="vid" cel="sibl.0anto.FON">siblanto<sncref/></ref>
    <trd lng="be">&c_sh;&c_y;&c_p;&c_ja;&c_ch;&c_y; &c_z;&c_y;&c_ch;&c_n;&c_y;</trd>
    <trdgrp lng="en">
      <trd>hushing sibilant</trd>, <trd>shibilant</trd>
    </trdgrp>
    <trd lng="fr">chuintante<klr> (phon.)</klr></trd>
    <trdgrp lng="hu">
      <trd>r&eacute;shang</trd>, <trd>spir&aacute;ns</trd>
    </trdgrp>
    <trdgrp lng="it">
<trd><klr>consonante </klr><ind>fricativa</ind> postalveolare</trd>, 
<trd><klr>consonante </klr><ind>sibilante</ind> postalveolare</trd>
    </trdgrp>
    <trd lng="nl">sisklank</trd>
    <trd lng="ru">&c_sh;&c_i;&c_p;&c_ja;&c_shch;&c_a;&c_ja; &c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_n;&c_a;&c_ja;</trd>
  <trd lng="uk">&#x448;&#x438;&#x43F;&#x43B;&#x44F;&#x447;&#x438;&#x439; &#x43F;&#x440;&#x438;&#x433;&#x43E;&#x43B;&#x43E;&#x441;&#x43D;&#x438;&#x439;</trd>
</drv>

<drv mrk="sxusx.0o">
    <kap><tld/>o</kap>
    <snc>
      <dif>
        &Scirc;-<ref tip="lst" cel="son.0o" lst="voko:sonoj">sona</ref> bruo:
        <ekz>
          <tld/>ado de furioza katino a&ubreve;di&gcirc;is en la &ccirc;ambro
          <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 24a, p. 250a</lok></fnt>.
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="be">
      <trd>&c_sh;&c_o;&c_r;&c_g;&c_a;&c_t;</trd>,
      <trd>&c_sh;&c_o;&c_r;&c_a;&c_h;</trd>,
      <trd>&c_sh;&c_a;&c_m;&c_a;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_sh;&c_a;&c_p;&c_a;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_sh;&c_a;&c_s;&c_t;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trd lng="de">Zischen</trd>
    <trdgrp lng="fr">
      <trd>bruissement</trd>,
      <trd>chuintement</trd>
    </trdgrp>
   <trdgrp lng="it">
      <trd>fischio</trd>,
      <trd>sibilo</trd>,
      <trd>sfrigolio</trd>,
      <trd>il <ind>soffiare</ind><klr> (verso del gatto)</klr></trd>
    </trdgrp>
    <trd lng="nl">sissend geluid</trd>
    <trdgrp lng="ru">
      <trd>&c_sh;&c_i;&c_p;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_sh;&c_o;&c_r;&c_o;&c_h;</trd>,
      <trd>&c_sh;&c_je;&c_l;&c_je;&c_s;&c_t;</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>brus</trd>,
      <trd>fr&auml;sande<klr> (ljud)</klr></trd>
    </trdgrp>
  <trd lng="uk">&#x448;&#x438;&#x43F;&#x456;&#x43D;&#x43D;&#x44F;</trd>
</drv>
</art>
