<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>&scirc;u&scirc;</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sxusx.0i">
    <kap><tld/>i</kap>
    <gra><vspec>ntr</vspec></gra>
    <snc>
      <dif>
        <ref tip="super" cel="bru.0i.soni">Brui</ref> frote, kun tirata
        &scirc;-sono:
        <ekz>
          oleo <tld/>as sur la pato;
        </ekz>
        <ekz>
          varmega fero <tld/>as en la akvo;
        </ekz>
        <ekz>
          La mineral'akvo <tld/>anta en la gor&gcirc;o<fnt><bib>MkM</bib>,
          <lok>&ccirc;apitro 5a, p. 48a</lok></fnt>!
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="fr">
      <trd>bruire</trd>,
      <trd>bruisser</trd>,
      <trd>chuinter</trd>
    </trdgrp>
</drv>

<drv mrk="sxusx.0a.konsonanto">
    <kap><tld/>a konsonanto</kap>
    <uzo tip="fak">FON</uzo>
    <snc>
      <dif>
        <ref tip="super" cel="frikat.0o">Frikativo</ref> (frota konsonanto)
        el la serio <ctl>&scirc;</ctl>, <ctl>&jcirc;</ctl>, a&uuml; el aliaj, palataj sonoj:
        <ekz>
        </ekz>
      </dif>
    </snc>
    <trd lng="fr">chuintante<klr> (phon.)</klr></trd>
</drv>

<drv mrk="sxusx.0o">
    <kap><tld/>o</kap>
    <snc>
      <dif>
        &Scirc;-sona bruo:
        <ekz>
          <tld/>ado de furioza katino a&ubreve;di&gcirc;is en la
          &ccirc;ambro
          <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 24a, p. 250a</lok></fnt>;
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="fr">
      <trd>bruissement</trd>,
      <trd>chuintement</trd>
    </trdgrp>
</drv>
</art>
