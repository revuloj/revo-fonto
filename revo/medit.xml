<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>medit</rad>/i
</kap>
<drv mrk="medit.0i">
  <kap>
    <ofc>*</ofc><tld/>i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Longe fiksi kaj koncentrigi sian penson sur io; enprofundi&gcirc;i en
      pensado:
      <ekz>
        stomako malsata nur pri pano <tld/>as<fnt>Z</fnt>;
      </ekz>
      <ekz>
        fremda <tld/>o estas ka&scirc;ita<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="pens.pri0i">pripensi</ref>,
      <ref cel="cerb.0umi">cerbumi</ref>.
    </refgrp>
  </snc>
  <trd lng="en">meditate</trd>
  <trdgrp lng="nl">
    <trd>pijnzen</trd>,
    <trd>mediteren</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_m;&c_y;&c_sh;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_m;&c_je;&c_d;&c_i;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_u;&c_m;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
