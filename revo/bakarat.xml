<vortaro>
<art mrk="$Id: bakarat.xml,v 1.3 2023/03/06 09:30:51 afido Exp $">
<kap>
  <rad>bakarat</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="bakarat.0o">
  <kap><tld/>o</kap>
  <snc mrk="bakarat.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="super" cel="kart.0ludo.SPO">Kartludo</ref> populara en vetludejoj,
      en kiu ludas bankiero kaj vetantoj:
      <ekz>
        <nom>Aleksander Pu&scirc;kin</nom> <klr>[&#8230;]</klr> ludis en la jaro 1821 kun 
        oficiro <tld/>on kaj supozis trompon, &ccirc;ar li da&ubreve;re
        perdis la ludojn
        <fnt>
         <aut>H.-G. Kaiser</aut>
         <vrk><url
         ref="http://karapacopanoramo.blogspot.fr/2007/05/kio-tiel-ne-legeblas-en-leksikono.html">
         Pri popularsciencaj aferoj...</url></vrk>
         <lok>Karapacopanoramo, 2007-05-29</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_k;&c_a;&c_r;&c_a; <klr tip="amb">(&c_g;&c_u;&c_l;&c_mol;&c_n;&c_ja;)</klr></trd>
  <trd lng="fr">baccara</trd>
  <trd lng="uk">&c_b;&c_a;&#x43A;&c_a;&c_r;&c_a;</trd>
</drv>
</art>

