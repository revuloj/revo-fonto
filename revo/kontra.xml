  <rad>kontraband</rad>/i
</kap>

<drv mrk="kontra.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kontra&ubreve;le&gcirc;e enlandigi komerca&jcirc;ojn:
      <ekz>
        <klr>[&scirc;i]</klr> kontrabandis skotan viskion al Norvegio
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 33</lok>
        </fnt>;
      </ekz>
      <ekz>
        en Hamburgo maristoj <tld/>is al lokaj esperantistoj kontra&ubreve;le&gcirc;ajn bro&scirc;urojn
        <fnt>
          <bib>EeP</bib>
          <lok>Kvina Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la gasto <klr>[&#x2026;]</klr> profitante senzorgemon kaj malkleron de la redaktoro,
        provis <tld/>i <klr>(<ref cel="sxov.sub0i">sub&scirc;ovi</ref>)</klr> en la gazetaron 
        apologion de Jesuo Kristo
        <fnt>
          <bib>MkM</bib>
          <lok>13</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        virusojn la malamiko <tld/>as en komputilajn retojn por kolapsigi ilin
        <fnt><aut>Stefan Maul</aut>:
        <vrk>Militado sen bataliloj</vrk>, <bib>Monato</bib><lok>jaro
        2001a, numero 1a, p. 6a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="import.0i.EKON">importi</ref>
  </snc>
  <trd lng="be">&c_u;&c_v;&c_o;&c_z;&c_ib;&c_c;&c_mol; <ind>&c_k;&c_a;&c_n;&c_t;&c_r;&c_a;&c_b;&c_a;&c_n;&c_d;&c_a;&c_j;</ind></trd>
  <trdgrp lng="cs">
    <trd>pa&scaron;ovat</trd>,
    <trd>podloudni&ccaron;it</trd>,
    <trd>propa&scaron;ovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schmuggeln</trd>,
    <trd>einschleusen</trd>
  </trdgrp>
  <trd lng="es">contrabandear</trd>
  <trd lng="fa">&a_q;&a_A;&f_ch;&a_A;&a_q; &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>faire la <ind>contrebande</ind></trd>
  </trdgrp>
  <trd lng="hu">csemp&eacute;szik</trd>
  <trdgrp lng="ja">
    <trd>&#x5BC6;&#x8F38;&#x5165;&#x3059;&#x308B;  <pr>&#x307F;&#x3064;&#x3086;&#x306B;&#x3085;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5BC6;&#x8F38;&#x3059;&#x308B;  <pr>&#x307F;&#x3064;&#x3086;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">smokkelen</trd>
  <trd lng="pl">przemyca&cacute;</trd>     
  <trd lng="ro">face contraband&abreve;</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_v;&c_o;&c_z;&c_i;&c_t;&c_mol;
  <ind>&c_k;&c_o;&c_n;&c_t;&c_r;&c_a;&c_b;&c_a;&c_n;&c_d;&c_o;&c_j;</ind></trd>
  <trd lng="sk">prepa&scaron;ova&tcaron;</trd>
  <trdgrp lng="zh">
    <trd>&#x8D70;&#x79C1; <pr>z&#x1D2;us&#x12B;</pr></trd>,
    <trd>&#x8D70;&#x79C1;&#x6F0F;&#x7A05; <pr>z&#x1D2;us&#x12B;l&#xF2;ushu&#xEC;</pr></trd>,
    <trd>&#x79C1;&#x8FD0; <pr>s&#x12B;y&#xF9;n</pr></trd>,
    <trd>&#x5077;&#x8FD0; <pr>t&#x14D;uy&#xF9;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kontra.0o">
  <kap><ofc>2</ofc><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Sekreta <ref tip="super" cel="import.0ado">importado</ref>
      de komerca&jcirc;oj spite al le&gcirc;a malpermeso
      a&ubreve; sen pago de <ref tip="vid" cel="dogan.0o">dogano</ref>:
      <ekz>
        pro tabaka <tld/>o en Britio kutime oni punas per &gcirc;is kvar jaroj da mallibero
        <fnt>
	<aut>Laimius Stra&zcaron;nickas</aut>
	<vrk><url ref="&Monato;/2000/006830.php">Kontrabanditaj cigaredoj invadas Brition</url></vrk>
	<bib>Monato</bib>
	<lok>2000/04, p. 13</lok>
        </fnt>;
      </ekz>
      <ekz>
        kontra&ubreve;batalo de narkota&jcirc;o-<tld/>o el Afganio
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2012/010643.php">Kreskas narkota&jcirc;o-komerco</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkmiloj, kiuj jam atingis E&ubreve;ropon, estos revenigitaj kiel kontra&ubreve;le&gcirc;aj
        enmigrintoj, tiel komenci&gcirc;os denove la homa <tld/>o-karuselo
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2015/011487.php">Facilas priskribi, malfacilas preskribi</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_t;&c_r;&c_a;&c_b;&c_a;&c_n;&c_d;&c_a;
  <klr>(&c_d;&c_z;&c_je;&c_j;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;)</klr></trd>
  <trdgrp lng="ca">
    <trd>contraban</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kontraband</trd>,
    <trd>pa&scaron;</trd>,
    <trd>pa&scaron;ovan&eacute; zbo&zcaron;&iacute;</trd>
  </trdgrp>
  <trd lng="de">Schmuggel</trd>
  <trdgrp lng="en">
    <trd>contraband</trd>,
    <trd>prohibited traffic</trd>,
    <trd>smuggling</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>contrabando</trd>
  </trdgrp>
  <trd lng="fa">&a_q;&a_A;&f_ch;&a_A;&a_q;</trd>
  <trdgrp lng="fr">
    <trd>contrebande</trd>
  </trdgrp>
  <trd lng="hu">csemp&eacute;szet</trd>
  <trdgrp lng="ja">
    <trd>&#x5BC6;&#x8F38;&#x5165;  <pr>&#x307F;&#x3064;&#x3086;&#x306B;&#x3085;&#x3046;</pr></trd>,
    <trd>&#x5BC6;&#x8CBF;&#x6613;  <pr>&#x307F;&#x3064;&#x307C;&#x3046;&#x3048;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="nl">smokkelhandel</trd>
  <trdgrp lng="pl">
    <trd>przemyt</trd>,
    <trd>kontrabanda</trd>,
    <trd>szmugiel [pot]</trd>
  </trdgrp> 
  <trd lng="pt">contrabando</trd>
  <trd lng="ro">contraband&abreve;</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_t;&c_r;&c_a;&c_b;&c_a;&c_n;&c_d;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>pa&scaron;er&aacute;ctvo</trd>,
    <trd>pa&scaron;ovanie</trd>
  </trdgrp>
  <trd lng="zh">&#x8D70;&#x79C1;&#x6F0F;&#x7A05; <pr>z&#x1D2;us&#x12B;l&#xF2;ushu&#xEC;</pr></trd>
</drv>


<drv mrk="kontra.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="komerc.0ajxo">Komerca&jcirc;o</ref> kontra&ubreve;le&gcirc;e enlandigita:
      <ekz>
        trajneto kun 16 vagonetoj por transporti <tld/>a&jcirc;ojn (&ccirc;efe cigaredojn sed anka&ubreve;
        eble homojn el evoluolandoj)
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2012/010819.php">Kontrabandistoj elreligitaj</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        la britoj <klr>[&#x2026;]</klr> kontra&ubreve;le&gcirc;e eniris iranan marspacon, kiam ili kontrolis
        pro <tld/>a&jcirc;oj var&scirc;ipon
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2007/009225.php">Kiom valoras kaptiteco?</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ebloj ke Albanio fari&gcirc;u transira lando de krimeco kaj
        <tld/>a&jcirc;oj de orienta al okcidenta E&ubreve;ropo
        <fnt><aut>Bardhyl Selimi</aut>: <vrk>Albanio, la homa disvolvi&gcirc;o dum la lastaj
        dek jaroj</vrk>, <bib>Monato</bib><lok>jaro 2002a, numero 3a, p.
        12-14a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_t;&c_r;&c_a;&c_b;&c_a;&c_n;&c_d;&c_a; <klr>(&c_t;&c_a;&c_v;&c_a;&c_r;)</klr></trd>
  <trdgrp lng="ca">
    <trd>g&egrave;nere de contraban</trd>,
    <trd>matuta</trd>
  </trdgrp>
  <trd lng="de">Schmuggelware</trd>
  <trdgrp lng="es">
    <trd>contrabando</trd>
    <trd>mercanc&iacute;a de contrabando</trd>,
  </trdgrp>
  <trd lng="fa">&f_k;&a_A;&a_l;&a_A;&f_y; <ind>&a_q;&a_A;&f_ch;&a_A;&a_q;</ind></trd>
  <trdgrp lng="fr">
    <trd>marchandise de <ind>contrebande</ind></trd>
  </trdgrp>
  <trd lng="hu">csemp&eacute;sz&aacute;ru</trd>    
  <trdgrp lng="ja">
    <trd>&#x5BC6;&#x8F38;&#x54C1;  <pr>&#x307F;&#x3064;&#x3086;&#x3072;&#x3093;</pr></trd>,
    <trd>&#x7981;&#x5236;&#x54C1;  <pr>&#x304D;&#x3093;&#x305B;&#x3044;&#x3072;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">smokkelwaar</trd>
  <trdgrp lng="pl">
    <trd>kontrabanda</trd>,
    <trd>przemyt</trd>
  </trdgrp>
  <trd lng="ro">contraband&abreve;</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_t;&c_r;&c_a;&c_b;&c_a;&c_n;&c_d;&c_a;<klr tip="amb">
  (&c_t;&c_o;&c_v;&c_a;&c_r;)</klr></trd>
  <trdgrp lng="zh">
    <trd>&#x79C1;&#x8CA8; <pr>s&#x12B;hu&#xF2;</pr></trd>,
    <trd>&#x6C34;&#x8CA8; <pr>shu&#x1D0;hu&#xF2;</pr></trd>,
    <trd>&#x8D70;&#x79C1;&#x8CA8; <pr>z&#x1D2;us&#x12B;hu&#xF2;</pr></trd>,
    <trd>&#x9ED1;&#x8CA8; <pr>h&#x113;ihu&#xF2;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kontra.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Homo kiu okaze a&ubreve; vivtene <tld/>as:
      <ekz>
        <tld/>istoj kaj aliaj kun aliaj negocoj, <klr>[&#x2026;]</klr> pri kiuj oni e&ccirc; ne
        a&ubreve;dacus pensi
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 33</lok>
        </fnt>;
      </ekz>
      <ekz>
        Serbio situas sur balkana narkota&jcirc;o-vojo kaj la polico multe laboras kontra&ubreve;
        <tld/>istoj
        <fnt>
          <bib>Monato</bib>
          <aut>Dimitrije Jani&ccaron;i&cacute;</aut>
          <vrk><url ref="&Monato;/2009/009807.php">Heroino-herooj</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        hom-<tld/>istoj, pretaj riski la vivojn de la &leftquot;klientoj&rightquot; por ilin transmarigi el
        la &hcirc;aose senespera norda Afriko al la promesita lando de E&ubreve;ropa Unio
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2015/011487.php">Facilas priskribi, malfacilas preskribi</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_t;&c_r;&c_a;&c_b;&c_a;&c_n;&c_d;&c_y;&c_s;&c_t;</trd>
  <trd lng="ca">contrabandista</trd>
  <trdgrp lng="cs">
    <trd>pa&scaron;er&aacute;k</trd>,
    <trd>podloudn&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schmuggler</trd>,
    <trd>Schleuser</trd>
  </trdgrp>
  <trd lng="es">contrabandista</trd>
  <trd lng="fa">&a_q;&a_A;&f_ch;&a_A;&a_q;&f_ch;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>contrebandier</trd>
  </trdgrp>
  <trd lng="hu">csemp&eacute;sz</trd>
  <trd lng="ja">&#x5BC6;&#x8F38;&#x696D;&#x8005;  <pr>&#x307F;&#x3064;&#x3086;&#x304E;&#x3087;&#x3046;&#x3057;&#x3083;</pr></trd>
  <trd lng="nl">smokkelaar</trd>
  <trd lng="pl">przemytnik</trd>     
  <trd lng="ro">contrabandist</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_t;&c_r;&c_a;&c_b;&c_a;&c_n;&c_d;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">pa&scaron;er&aacute;k</trd>
  <trdgrp lng="zh">
    <trd>&#x67AD; <pr>xi&#x101;o</pr></trd>,
    <trd>&#x67AD;&#x532A; <pr>xi&#x101;of&#x11B;i</pr></trd>,
    <trd>&#x79C1;&#x67AD; <pr>s&#x12B;xi&#x101;o</pr></trd>,
    <trd>&#x8D70;&#x79C1;&#x72AF; <pr>z&#x1D2;us&#x12B;f&#xE0;n</pr></trd>,
    <trd>&#x8D70;&#x79C1;&#x8005; <pr>z&#x1D2;us&#x12B;zh&#x11B;</pr></trd>
  </trdgrp>
</drv>
</art>
