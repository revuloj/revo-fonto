<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kontra.xml,v 1.21 2021/02/14 20:10:34 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>kontraband</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kontra.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Sekreta <ref tip="super" cel="import.0ado">importado</ref>
      de komerca&jcirc;oj spite al le&gcirc;a malpermeso
      a&ubreve; sen pago de <ref tip="vid" cel="dogan.0o">dogano</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_t;&c_r;&c_a;&c_b;&c_a;&c_n;&c_d;&c_a;
  <klr>(&c_d;&c_z;&c_je;&c_j;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;)</klr></trd>
  <trdgrp lng="ca">
    <trd>contraban</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kontraband</trd>,
    <trd>pa&scaron;</trd>,
    <trd>pa&scaron;ovan&eacute; zbo&zcaron;&iacute;</trd>
  </trdgrp>
  <trd lng="de">Schmuggel</trd>
  <trdgrp lng="en">
    <trd>contraband</trd>,
    <trd>prohibited traffic</trd>,
    <trd>smuggling</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>contrabando</trd>
  </trdgrp>
  <trd lng="fa">&a_q;&a_A;&f_ch;&a_A;&a_q;</trd>
  <trdgrp lng="fr">
    <trd>contrebande</trd>
  </trdgrp>
  <trd lng="hu">csemp&eacute;szet</trd>
  <trd lng="nl">smokkelhandel</trd>
  <trdgrp lng="pl">
    <trd>przemyt</trd>,
    <trd>kontrabanda</trd>,
    <trd>szmugiel [pot]</trd>
  </trdgrp> 
  <trd lng="pt">contrabando</trd>
  <trd lng="ro">contraband&abreve;</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_t;&c_r;&c_a;&c_b;&c_a;&c_n;&c_d;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>pa&scaron;er&aacute;ctvo</trd>,
    <trd>pa&scaron;ovanie</trd>
  </trdgrp>
</drv>

<drv mrk="kontra.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kontra&ubreve;le&gcirc;e enlandigi komerca&jcirc;ojn:
      <ekz>
        <uzo tip="stl">FIG</uzo>virusojn la malamiko <tld/>as en
        komputilajn retojn por kolapsigi ilin<fnt><aut>Stefan
        Maul</aut>:
        <vrk>Militado sen bataliloj</vrk>, <bib>Monato</bib><lok>jaro
        2001a, numero 1a, p. 6a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>pa&scaron;ovat</trd>,
    <trd>podloudni&ccaron;it</trd>,
    <trd>propa&scaron;ovat</trd>
  </trdgrp>
  <trd lng="de">schmuggeln</trd>
  <trd lng="es">contrabandear</trd>
  <trd lng="fa">&a_q;&a_A;&f_ch;&a_A;&a_q; &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>faire la <ind>contrebande</ind></trd>
  </trdgrp>
  <trd lng="hu">csemp&eacute;szik</trd>
  <trd lng="nl">smokkelen</trd>
  <trd lng="pl">przemyca&cacute;</trd>     
  <trd lng="ro">face contraband&abreve;</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_v;&c_o;&c_z;&c_i;&c_t;&c_mol;
  <ind>&c_k;&c_o;&c_n;&c_t;&c_r;&c_a;&c_b;&c_a;&c_n;&c_d;&c_o;&c_j;</ind></trd>
  <trd lng="sk">prepa&scaron;ova&tcaron;</trd>
</drv>

<drv mrk="kontra.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Komerca&jcirc;o kontra&ubreve;le&gcirc;e enlandigita:
      <ekz>
        la ebloj ke Albanio fari&gcirc;u transira lando de krimeco kaj
        <tld/>a&jcirc;oj de orienta al okcidenta E&ubreve;ropo<fnt><aut>Bardhyl
        Selimi</aut>: <vrk>Albanio, la homa disvolvi&gcirc;o dum la lastaj
        dek
        jaroj</vrk>, <bib>Monato</bib><lok>jaro 2002a, numero 3a, p.
        12-14a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_t;&c_r;&c_a;&c_b;&c_a;&c_n;&c_d;&c_a; <klr>(&c_t;&c_a;&c_v;&c_a;&c_r;)</klr></trd>
  <trdgrp lng="ca">
    <trd>g&egrave;nere de contraban</trd>,
    <trd>matuta</trd>
  </trdgrp>
  <trd lng="de">Schmuggelware</trd>
  <trdgrp lng="es">
    <trd>contrabando</trd>
    <trd>mercanc&iacute;a de contrabando</trd>,
  </trdgrp>
  <trd lng="fa">&f_k;&a_A;&a_l;&a_A;&f_y; <ind>&a_q;&a_A;&f_ch;&a_A;&a_q;</ind></trd>
  <trdgrp lng="fr">
    <trd>marchandise de <ind>contrebande</ind></trd>
  </trdgrp>
  <trd lng="hu">csemp&eacute;sz&aacute;ru</trd>    
  <trd lng="nl">smokkelwaar</trd>
  <trdgrp lng="pl">
    <trd>kontrabanda</trd>,
    <trd>przemyt</trd>
  </trdgrp>
  <trd lng="ro">contraband&abreve;</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_t;&c_r;&c_a;&c_b;&c_a;&c_n;&c_d;&c_a;<klr tip="amb">
  (&c_t;&c_o;&c_v;&c_a;&c_r;)</klr></trd>
</drv>

<drv mrk="kontra.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Homo kiu okaze a&ubreve; vivtene kontrabandas.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_t;&c_r;&c_a;&c_b;&c_a;&c_n;&c_d;&c_y;&c_s;&c_t;</trd>
  <trd lng="ca">contrabandista</trd>
  <trdgrp lng="cs">
    <trd>pa&scaron;er&aacute;k</trd>,
    <trd>podloudn&iacute;k</trd>
  </trdgrp>
  <trd lng="de">Schmuggler</trd>
  <trd lng="es">contrabandista</trd>
  <trd lng="fa">&a_q;&a_A;&f_ch;&a_A;&a_q;&f_ch;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>contrebandier</trd>
  </trdgrp>
  <trd lng="hu">csemp&eacute;sz</trd>
  <trd lng="nl">smokkelaar</trd>
  <trd lng="pl">przemytnik</trd>     
  <trd lng="ro">contrabandist</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_t;&c_r;&c_a;&c_b;&c_a;&c_n;&c_d;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">pa&scaron;er&aacute;k</trd>
</drv>
</art>
