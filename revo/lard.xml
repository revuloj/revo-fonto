<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lard</rad>/o
</kap>
<drv mrk="lard.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Subha&ubreve;ta tavolo da graso, &ccirc;e porko; porkograso.
    </dif>
    <refgrp tip="sin">
      <ref cel="seb.0o">sebo</ref>.
    </refgrp>
  </snc>
  <trd lng="el">&lambda;&alfa;&rho;&delta;&jota_ton;</trd>
  <trd lng="en">bacon</trd>
  <trd lng="hu">szalonna</trd>
  <trd lng="nl">spek</trd>
  <trdgrp lng="pt">
    <trd>lardo</trd>,
    <trd>toicinho</trd>,
    <trd>toucinho</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_a;&c_l;&c_o;</trd>
</drv>

<drv mrk="lard.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Enpiki <tld/>erojn en viandon.
    </dif>
  </snc>
  <trd lng="en">lard</trd>
  <trd lng="hu">szalonn&aacute;val <ind>sp&eacute;kel</ind></trd>
  <trd lng="nl">larderen</trd>
  <trd lng="pt">lardear</trd>
</drv>
</art>
