<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: kambium.xml,v 1.3 2025/12/27 22:34:01 afido Exp $">
    <kap>
      <rad>kambium</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="kambium.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Histotavolo &ccirc;e dukotiledonaj vegeta&jcirc;oj, kiu kreskigas la diametron de tigoj kaj radikoj;
      &ccirc;e arboj &gcirc;i trovi&gcirc;as inter <ref tip="prt" cel="alburn.0o">alburno</ref> kaj
      <ref tip="prt" cel="sxel.0o">&scirc;elo</ref>:
      <ekz>
        en arboj <klr>[&#8230;]</klr> la <tld/>o permesas la vasti&gcirc;on de vaskula histo kiu
        produktas lignan kreskon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=310"
              >Arbo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Kambium</trd>
    <trd lng="en">vascular cambium</trd>
    <trd lng="fr">cambium </trd>
    <trdgrp lng="pl">
      <trd>kambium</trd>,
      <trd>miazga tw&oacute;rcza</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_a;&c_m;&c_b;&c_i;&c_j;</trd>
  </snc>
<trd lng="be">&c_m;&c_ja;&c_z;&c_g;&c_a; <klr tip="amb">(&c_b;&c_a;&c_t;.)</klr></trd>
</drv>
</art>

