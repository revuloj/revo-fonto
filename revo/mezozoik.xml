<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mezozoik.xml,v 1.15 2023/10/19 15:13:16 revo Exp $">
<kap>
  <rad>mezozoik</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="mezozoik.0o">
  <kap><tld/>o</kap>
  <snc mrk="mezozoik.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <uzo tip="fak">PAL</uzo>
    <dif>
      La 2a <ref tip="lst" cel="era.0o.GEOL" lst="voko:geologiaj_eraoj" val="-251000000">erao</ref>
      de <ref tip="malprt" cel="fanerozoik.0o.GEOL">fanerozoiko</ref>,
      inkludanta periodojn:
      <ref tip="prt" cel="kretac.0o">kretaceo</ref>,
      <ref tip="prt" cel="jxuras.0o">&jcirc;uraso</ref> kaj
      <ref tip="prt" cel="trias.0o">triaso</ref>.
      <ekz>
        la <tld/>a erao da&ubreve;ris anta&ubreve; &ccirc;irka&ubreve;
        inter 225 kaj 70 milionoj da jaroj;
      </ekz>
      <ekz>
        la mezozoiko, ronde &gcirc;is anta&ubreve; 60 milionoj da jaroj, estas la a&gcirc;o de reptilioj
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estas nomata &leftquot;mezepoko&rightquot; de Tero.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kenozoik.0o.GEOL">kenozoiko</ref>,
      <ref cel="paleozoik.0o.GEOL">paleozoiko</ref>,
      <ref cel="proterozoik.0o.GEOL">proterozoiko</ref>,
      <ref cel="arkean.0o.GEOL">arkeano</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_e;&c_z;&c_a;&c_z;&c_o;&c_j;</trd>,
    <trd>&c_m;&c_e;&c_z;&c_a;&c_z;&c_o;&c_j;&c_s;&c_k;&c_a;&c_ja; &c_e;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">mesozoic</trd>
  <trdgrp lng="cs">
    <trd>druhohory</trd>,
    <trd>mezozoikum</trd>
  </trdgrp>
  <trd lng="de">Mesozoikum</trd>
  <trd lng="en">mesozoic</trd>
  <trd lng="fr">M&eacute;sozo&iuml;que</trd>
  <trdgrp lng="ja">
    <trd>&#x4E2D;&#x751F;&#x4EE3;  <pr>&#x3061;&#x3085;&#x3046;&#x305B;&#x3044;&#x3060;&#x3044;</pr></trd>,
    <trd>&#x4E2D;&#x751F;&#x754C;  <pr>&#x3061;&#x3085;&#x3046;&#x305B;&#x3044;&#x304B;&#x3044;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>druhohory</trd>,
    <trd>mezozoikum</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>mezozoik</trd>,
    <trd>era <ind>mezozoiczna</ind></trd>
  </trdgrp>
  <trd lng="pt">Mesoz&oacute;ico</trd>
  <trd lng="uk">&c_m;&c_je;&c_z;&c_o;&c_z;&c_o;&c_j;&c_s;&c_mol;&c_k;&c_a; &c_je;&c_r;&c_a;</trd>
</drv>
</art>

