<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mezozoik</rad>/o
  <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="mezozoik.0o">
  <kap><tld/>o</kap>
  <snc mrk="mezozoik.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <uzo tip="fak">PAL</uzo>
    <dif>
      La 2a <ref tip="super" cel="era.0o.GEOL">erao</ref>
      de <ref tip="super" cel="fanerozoik.0o.GEOL">fanerozoiko</ref>,
      inkludanta periodojn:
      <ref tip="sub" cel="kretac.0o">kretaceo</ref>,
      <ref tip="sub" cel="jxuras.0o">&jcirc;urasio</ref> kaj 
      <ref tip="sub" cel="trias.0o">triaso</ref>.
      <ekz>
        <tld/>a erao da&ubreve;ris anta&ubreve; &ccirc;irka&ubreve; 
        inter 225 kaj 70 milionoj da  jaroj;
      </ekz>
      <ekz>
        <tld/>o estas nomtata "mezepoko" de Tero.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kenozoik.0o.GEOL">kenozoiko</ref>, 
      <ref cel="paleozoik.0o.GEOL">paleozoiko</ref>, 
      <ref cel="proterozoik.0o.GEOL">proterozoiko</ref>, 
      <ref cel="arkean.0o.GEOL">arkeano</ref>.
    </refgrp>
    <trd lng="en">mesozoic</trd>
    <trdgrp lng="pl">
      <trd>mezozoik</trd>,
      <trd>era <ind>mezozoiczna</ind></trd>
    </trdgrp>
  </snc>
</drv>
</art>

