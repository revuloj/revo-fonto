<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>alte</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="alte.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="alte.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="malvac.0oj">malvacoj</ref>
      (<trd lng="la">Althaea</trd>).
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Althee</trd>,
    <trd>Eibisch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Althea</trd>,
    <trd>hibiscus</trd>
  </trdgrp>
  <trd lng="fr">althaea</trd>
  <trd lng="hu">ziliz</trd>
  <trd lng="nl">heemst</trd>
  <trdgrp lng="pt">
    <trd>alt&eacute;ia</trd>,
    <trd>malva&iacute;sco</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_l;&c_t;&c_je;&c_j;</trd>
</drv>

<drv mrk="alte.roz0o">
  <kap>roz<tld/>o</kap>
  <snc mrk="alte.roz0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <tld lit="A"/>o kun altaj tigoj kaj belaj floroj blankaj a&#365; 
      purpuraj, du- a&#365; plur-jara (<trd lng="la">Althaea rosea</trd>):
      <ekz>
        per dika mano /
        responde montras vojon &#8212; /
        jen, roz<tld/>o
        <fnt>
          <aut>Vastalto</aut>,
          <vrk><url ref="http://www.vastalto.com/blog">Akvosfero</url></vrk>,
          <lok>2007-05-16</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">rose tr&#233;mi&#232;re</trd>
</drv>
</art>
