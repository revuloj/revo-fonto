<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: alte.xml,v 1.32 2023/10/28 08:33:05 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>alte</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="alte.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="alte.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="malvac.0oj">malvacoj</ref>
      (<trd lng="la">Althaea</trd>):
      <ekz>
        la <tld/>o estas rekte staranta sta&ubreve;do kun fortaj tigoj, kiu
        krome estas valora
        abelnutra planto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=24389">Alteo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/5/57/Althaea_officinalis04.jpg/360px-Althaea_officinalis04.jpg" prm="CC-BY-SA-3.0">floro kaj folioj de <tld/>o (Althaea officinalis)
        <fnt>
          <aut>Follavoine &ccirc;e fr.wikipedia</aut>
          <lok><url ref="&WCW;/File:Althaea_officinalis04.jpg">&WCW;/File:Althaea_officinalis04.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_sh;&c_l;&c_ja;&c_z;</trd>
  <trd lng="bg">&c_b;&c_ja;&c_l;&c_a;
  <ind>&c_r;&c_u;&c_zh;&c_a;</ind></trd>
  <trdgrp lng="ca">
    <trd>g&egrave;nere Althaea</trd>,
    <trd>malv&iacute;, altea</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ibi&scaron;ek</trd>,
    <trd>proskurn&iacute;k</trd>,
    <trd>sl&eacute;z</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Althee</trd>,
    <trd>Eibisch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Althea</trd>,
    <trd>hibiscus</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>althaea</trd>,
    <trd>altea</trd>
  </trdgrp>
  <trd lng="fr">althaea</trd>
  <trd lng="he">&#x5E0;&#x5D8;&#x5D5;&#x5E4;&#x5D9;&#x5EA;</trd>
  <trd lng="hu">ziliz</trd>
  <trd lng="ja">&#x30BF;&#x30C1;&#x30A2;&#x30AA;&#x30A4;</trd>
  <trd lng="nl">heemst</trd>
  <trdgrp lng="pt">
    <trd>alt&eacute;ia</trd>,
    <trd>malva&iacute;sco</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_l;&c_t;&c_je;&c_j;</trd>
  <trd lng="sk">ibi&scaron;tek</trd>
  <trd lng="uk">&c_a;&#x43B;&c_t;&c_je;&c_j;</trd>
</drv>

<drv mrk="alte.roz0o">
  <kap>roz<tld/>o</kap>
  <snc mrk="alte.roz0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="dif" cel="alce.0o">Alceo</ref>:
      <ekz>
        per dika mano / responde montras vojon &mdash; /
        jen, roz<tld/>o
        <fnt>
          <aut>Vastalto</aut>,
          <vrk><url ref="http://www.vastalto.com/blog/?m=200705&amp;paged=2">
          Akvosfero</url></vrk>,
          <lok>2007-05-16</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Stockmalve</trd>,
      <trd>Stockrose</trd>
    </trdgrp>
    <trd lng="zh">&#x8700;&#x8475; <pr>sh&#x1D4;ku&#xED;</pr></trd>
</snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_mol;&c_v;&c_a;</trd>
  <trd lng="ca">malva reial</trd>
  <trd lng="cs">proskurn&iacute;k topolovka</trd>
  <trd lng="en">hollyhock</trd>
  <trdgrp lng="es">
    <trd>malva loca</trd>,
    <trd>alcea</trd>,
    <trd>ca&ntilde;amera real</trd>,
    <trd>malcarrota</trd>,
    <trd>malmarriales</trd>,
    <trd>malva</trd>,
    <trd>malva Isabela</trd>,
    <trd>malva arb&oacute;rea</trd>,
    <trd>malva carm&iacute;n</trd>,
    <trd>malva de jard&iacute;n</trd>,
    <trd>malva de la princesa</trd>,
    <trd>malva de la reina</trd>,
    <trd>malva de las Indias</trd>,
    <trd>malva de los huertos</trd>,
    <trd>malva de los jardines</trd>,
    <trd>malva del pr&iacute;ncipe</trd>,
    <trd>malva doble</trd>,
    <trd>malva doncella</trd>,
    <trd>malva encendida</trd>,
    <trd>malva negra</trd>,
    <trd>malva real</trd>,
    <trd>malva real blanca</trd>,
    <trd>malva rosa</trd>,
    <trd>malva r&oacute;sea</trd>,
    <trd>malva sencilla</trd>,
    <trd>malvaloca</trd>,
    <trd>malvavisco</trd>,
    <trd>marvas</trd>,
    <trd>punz&oacute;</trd>,
    <trd>rosa africana</trd>,
    <trd>rosapalo</trd>,
    <trd>vara de San Jos&eacute;</trd>,
    <trd>Atea<klr> (Argentina)</klr></trd>,
    <trd>escarapela<klr> (Argentina)</klr></trd>,
    <trd>malva jaspeada<klr> (Argentina)</klr></trd>,
    <trd>Malvavisco de flor<klr> (Colombia)</klr></trd>,
    <trd>malv&oacute;n silvestre<klr> (Uruguay)</klr></trd>
  </trdgrp>
  <trd lng="fr">rose tr&eacute;mi&egrave;re</trd>
  <trd lng="hu">m&aacute;lyvar&oacute;zsa</trd>
  <trd lng="ja">&#x30BF;&#x30C1;&#x30A2;&#x30AA;&#x30A4;</trd>
  <trd lng="sk">topo&lcaron;ovka ru&zcaron;ov&aacute;</trd>
  <trd lng="uk">&#x448;&#x442;&#x43E;&#x43A;&#x440;&#x43E;&#x437;&#x430;</trd>
</drv>
</art>
