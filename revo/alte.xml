</kap>
<drv mrk="alte.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="malvac.0oj">malvacoj</ref>
      (<trd lng="la">Althaea</trd>).
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Althee</trd>,
    <trd>Eibisch</trd>
  </trdgrp>
  <trd lng="fr">althaea</trd>
  <trd lng="nl">heemst</trd>
  <trdgrp lng="pt">
    <trd>alt&eacute;ia</trd>,
    <trd>malva&iacute;sco</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_l;&c_t;&c_je;&c_j;</trd>
</drv>
</art>
