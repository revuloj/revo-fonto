<art mrk="$Id: aort.xml,v 1.24 2023/10/13 16:46:48 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>aort</rad>/o
</kap>
<drv mrk="aort.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      La precipa <ref tip="super" cel="arteri.0o.ANA">arterio</ref>,
      komenci&gcirc;anta &ccirc;e la <ref tip="vid" cel="kor.0o.ANA">koro</ref>:
      <ekz>
        embolioj povas formi&gcirc;i plej ofte en la koro pro kormalsanoj <klr>[&#x2026;]</klr> a&ubreve; el
        la materialo de arteriosklerozaj plaka&jcirc;oj en anta&ubreve;aj, pli grandaj arterioj
        kiel la <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=51997">Apopleksio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_o;&c_r;&c_t;&c_a;</trd>
  <trd lng="ca">aorta</trd>
  <trdgrp lng="cs">
    <trd>aorta</trd>,
    <trd>srde&ccaron;nice</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hauptschlagader</trd>,
    <trd>Aorta<klr> [fak.]</klr></trd>
  </trdgrp>
  <trd lng="en">aorta</trd>
  <trd lng="es">aorta</trd>
  <trd lng="fr">aorte</trd>
  <trd lng="he">&alef;&bet;&yod; &he;&ayin;&vav;&resh;&qof;&yod;&fmem;</trd>
  <trdgrp lng="hu">
    <trd>f&odblac;ver&odblac;&eacute;r</trd>,
    <trd>f&odblac;&uuml;t&odblac;&eacute;r</trd>,
    <trd>aorta</trd>
  </trdgrp>
  <trd lng="it">aorta</trd>
  <trd lng="ja">&#x5927;&#x52D5;&#x8108;  <pr>&#x3060;&#x3044;&#x3069;&#x3046;&#x307F;&#x3083;&#x304F;</pr></trd>
  <trd lng="nl">aorta</trd>
  <trd lng="pl">aorta</trd>
  <trd lng="ru">&c_a;&c_o;&c_r;&c_t;&c_a;</trd>
  <trd lng="sk">aorta</trd>
  <trd lng="uk">&#x430;&#x43E;&#x440;&#x442;&#x430;</trd>
</drv>
