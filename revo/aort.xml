  </snc>
  <trd lng="ca">aorta</trd>
  <trdgrp lng="de">
    <trd>Hauptschlagader</trd>,
    <trd>Aorta<klr> [fak.]</klr></trd>
  </trdgrp>
  <trd lng="en">aorta</trd>
  <trd lng="es">aorta</trd>
  <trd lng="fr">aorte</trd>
  <trdgrp lng="hu">
    <trd>f&odblac;ver&odblac;&eacute;r</trd>,
    <trd>f&odblac;&uuml;t&odblac;&eacute;r</trd>,
    <trd>aorta</trd>
  </trdgrp>
  <trd lng="nl">aorta</trd>
  <trd lng="ru">&c_a;&c_o;&c_r;&c_t;&c_a;</trd>
  <trd lng="sk">aorta</trd>
</drv>
</art>
