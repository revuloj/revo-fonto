  </trdgrp>
</drv>

<drv mrk="rul.de0i">
  <kap>de<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rul.de0i.KOMUNE">
    <dif>
      Forigi, faligi <tld/>ante:
      <ekz>
        <nom>Jakob</nom> <klr>[&#x2026;]</klr> de<tld/>is la &scirc;tonon de sur
        la aperturo de la puto
        <fnt><bib>MT</bib> <lok>&Gen; 29:10</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        hodia&ubreve; Mi de<tld/>is de vi la hontinda&jcirc;on de Egiptujo
        <fnt><bib>MT</bib> <lok>&Jos; 5:9</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_a;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">odvalit</trd>
  <trd lng="fr">rouler<klr tip="ind"> (tr., d'un
  endroit)</klr></trd>
  <trdgrp lng="hu">
    <trd>elg&ouml;rd&iacute;t</trd>,
    <trd>elgur&iacute;t</trd>,
    <trd>elhengerget</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>sturla&cacute;</trd>,
    <trd>wyturla&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_k;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>odg&uacute;&lcaron;a&tcaron;</trd>,
    <trd>odvali&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_ib;&c_d;&#x43A;&#x43E;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="rul.de0igxi">
  <kap>de<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="rul.de0igxi.KOMUNE">
    <dif>
      Fali <tld/>i&gcirc;ante:
      <ekz>
        sur &scirc;iaj vangoj de<tld/>i&gcirc;is pezaj larmoj
        <fnt><bib>Fab4</bib> <lok>Historio el la dunoj</lok></fnt>;
      </ekz>
      <ekz>
        de tempo al tempo ia guto de<tld/>i&gcirc;is kaj restis pendante inter la longaj trunketoj de la
        herbo
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        la akvo kun furioza bruego de<tld/>i&gcirc;as en defluejojn
        <fnt><bib>Metrop</bib> <lok>&ccirc;apitro 7a</lok></fnt>.
      </ekz>
    </dif>
    <rim>
      Iafoje videblas tiaj uzoj:
      <ekz>
        la interparolo glate de<tld/>i&gcirc;is
        <fnt><bib>UrdHadda</bib> <lok>p. 26a</lok></fnt>
      </ekz>
      kun la senco <ctl>okazi</ctl>, <ctl>pasi</ctl>,
      <ctl>funkcii</ctl>.
      Tio aspektas pa&ubreve;sa&jcirc;o de la franca lingvo kaj estas evitinda.
    </rim>
    <trd lng="de">abrollen</trd>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_a;&c_ch;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trd lng="fr">rouler<klr tip="ind"> (intr., d'un
  endroit)</klr></trd>
  <trdgrp lng="hu">
    <trd>leg&ouml;rd&uuml;l</trd>,
    <trd>legurul</trd>,
    <trd>lefolyik</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>sturla&cacute; si&eogonek;</trd>,
    <trd>wyturla&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_a;&c_t;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="rul.dis0i">
  <kap>
    dis<tld/>i
  </kap>
  <snc mrk="rul.dis0i.KOMUNE">
    <dif>
      Malvolvi, etendi rula&jcirc;on:
      <ekz>
        &scirc;i <klr>[&#x2026;]</klr> elprenis grandan kunkun<tld/>itan felon kaj dis<tld/>is &gcirc;in
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        malnovaj melodioj eksonis, apartaj taktoj, bildoj, ravantaj bildoj dis<tld/>i&gcirc;is
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_k;&c_r;&c_u;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">ausrollen</trd>
  <trd lng="fr">d&eacute;rouler</trd>
  <trd lng="pl">roztoczy&cacute;</trd>
</drv>

<drv mrk="rul.for0i">
  <kap>for<tld/>i</kap>
  <snc mrk="rul.for0i.KOMUNE">
    <dif>
      <tld lit="R"/>e forigi, formeti:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        vi for<tld/>as &scirc;tonon de mia koro
        <fnt>
          <aut>B. von Suttner, trad. A. Caumont kaj F. Luin</aut>
          <vrk>For la batalilojn!</vrk>
          <lok>1914, 2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">rouler<klr> (enlever en faisant rouler)</klr></trd>
</drv>

<drv mrk="rul.kun0i">
  <kap>kun<tld/>i</kap>
  <snc mrk="rul.kun0i.KOMUNE">
    <dif>
      <tld lit="R"/>i en unu tutecan <tld/>on:
      <ekz>
        <klr>[la]</klr> pentristo entrenis <klr>[&#x2026;]</klr> grandan pecon da kun<tld/>ita tolo
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la tago, en kiu Ni kun<tld/>os la
        &ccirc;ielojn, kiel kun<tld/>i&gcirc;as la skribvolva&jcirc;oj
        <fnt>
          <aut>trad. I. Chiussi</aut>
          <vrk>La Nobla Korano</vrk>
          <lok>1970</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">zusammenrollen</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_r;&c_u;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_v;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>srolovat</trd>,
    <trd>svinout</trd>
  </trdgrp>
  <trd lng="fr">enrouler</trd>
  <trd lng="ja">&#x5DFB;&#x304D;&#x3053;&#x3080;  <pr>&#x307E;&#x304D;&#x3053;&#x3080;</pr></trd>
  <trd lng="pl">zwin&aogonek;&cacute;</trd>
  <trd lng="sk">zvin&uacute;&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_g;&#x43E;&c_r;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&#x43A;&c_r;&c_u;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i; <klr>(&c_u; &c_r;&c_u;&#x43B;&#x43E;&#x43D;)</klr></trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6253;&#x6EDA; <pr>d&#x1CE;g&#x1D4;n</pr></trd>,
    <trd>&#x6253;&#x6EFE; <pr>d&#x1CE;g&#x1D4;n</pr></trd>,
    <trd>&#x5377; <pr>ju&#x1CE;n</pr></trd>,
    <trd>&#x6372; <pr>ju&#x1CE;n</pr></trd>,
    <trd>&#x7E08; <pr>y&#xED;ng</pr></trd>,
    <trd>&#x8426; <pr>y&#xED;ng</pr></trd>,
    <trd>&#x7E8F; <pr>ch&#xE1;n</pr></trd>,
    <trd>&#x7F20; <pr>ch&#xE1;n</pr></trd>,
    <trd>&#x87E0; <pr>p&#xE1;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rul.kun0igxi">
  <kap>kun<tld/>i&gcirc;i</kap>
  <snc mrk="rul.kun0igxi.KOMUNE">
    <dif>
      <tld lit="R"/>e &ccirc;irka&ubreve; si mem movi&gcirc;i a&ubreve; kunpremi&gcirc;i:
      <ekz>
        la knabino vidis knabon kun<tld/>i&gcirc;intan sur la benko
        <fnt><bib>DOz</bib>, <lok>&leftquot;la tertremo&rightquot;</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">sich <ind>zusammenrollen</ind></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_r;&c_u;&c_ch;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_mol;&c_v;&c_ib;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>pelotonner</ind></trd>,
    <trd>se <ind>rouler</ind> en boule</trd>
  </trdgrp>
  <trd lng="pl">zwija&cacute; si&eogonek; <klr>w k&lstroke;&eogonek;bek</klr></trd>
  <trdgrp lng="zh">
    <trd>&#x8737;&#x7E2E; <pr>qu&#xE1;nsu&#x14D;</pr></trd>,
    <trd>&#x8737;&#x7F29; <pr>qu&#xE1;nsu&#x14D;</pr></trd>,
    <trd>&#x62F3;&#x66F2; <pr>qu&#xE1;nq&#x16B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rul.kun0o">
  <kap>kun<tld/>o</kap>
  <snc mrk="rul.kun0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Samtempa plenumo de pluraj programoj, kunuzantaj komunajn
      rimedojn:
      <ekz>
        <nom>Rust</nom> estas &gcirc;eneralcela, plurparadigma, tradukita programlingvo kreita de
        <nom>Mozilla</nom> por konsistigi &leftquot;sekuran, kun<tld/>an, praktikan lingvon&rightquot;
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=483804">Rust (programlingvo)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_m;&c_a;&c_t;&c_z;&c_a;&c_d;&c_a;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_a;&c_l;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_je; &c_v;&c_y;&c_k;&c_a;&c_n;&c_a;&c_n;&c_mol;&c_n;&c_je; <klr>(&c_p;&c_r;&c_a;&c_g;&c_r;&c_a;&c_m;&c_a;&c_w;)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>soub&ecaron;&zcaron;nost</trd>,
    <trd>sou&ccaron;innost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Parallelausf&uuml;hrung</trd>,
    <trd>Threading</trd>
  </trdgrp>
  <trd lng="en">concurrency</trd>
  <trd lng="es">concurrencia</trd>
  <trd lng="hu">&ouml;sszeteker</trd>
  <trdgrp lng="ja">
    <trd>&#x540C;&#x6642;&#x5B9F;&#x884C;  <pr>&#x3069;&#x3046;&#x3058;&#x3058;&#x3063;&#x3053;&#x3046;</pr></trd>,
    <trd>&#x4E26;&#x884C;&#x51E6;&#x7406;  <pr>&#x3078;&#x3044;&#x3053;&#x3046;&#x3057;&#x3087;&#x308A;</pr></trd>
  </trdgrp>
  <trd lng="pl">wsp&oacute;&lstroke;bie&zdot;no&sacute;&cacute;</trd>
  <trd lng="ru">&c_m;&c_n;&c_o;&c_g;&c_o;&c_z;&c_a;&c_d;&c_a;&c_ch;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>s&uacute;be&zcaron;nos&tcaron;</trd>,
    <trd>s&uacute;&ccaron;innos&tcaron;</trd>
  </trdgrp>
</drv>

