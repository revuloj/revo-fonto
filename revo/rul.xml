    <trd>&#x8EE2;&#x304C;&#x3063;&#x3066;&#x884C;&#x304F;  <pr>&#x3053;&#x308D;&#x304C;&#x3063;&#x3066;&#x3044;&#x304F;</pr></trd>,
    <trd>&#x9032;&#x3080;  <pr>&#x3059;&#x3055;&#x3080;</pr></trd>,
    <trd>&#x3046;&#x306D;&#x3063;&#x3066;&#x9032;&#x3080;  <pr>&#x3046;&#x306D;&#x3063;&#x3066;&#x3059;&#x3059;&#x3080;</pr></trd>,
    <trd>&#x6A2A;&#x63FA;&#x308C;&#x3059;&#x308B;  <pr>&#x3088;&#x3053;&#x3086;&#x308C;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x6A2A;&#x8EE2;&#x3059;&#x308B;  <pr>&#x304A;&#x3046;&#x3066;&#x3093;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="ne">&#x930;&#x94B;&#x932;</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&c_t;&c_i;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&c_v;&c_i;&#x43A;&#x43E;&#x43D;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F; <klr>(&#x43F;&c_r;&#x43E;
    &#x43F;&c_r;&#x43E;&c_g;&c_r;&c_a;&#x43C;&c_u;)</klr></trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8EF2; <pr>g&#x16B;</pr></trd>,
