    <trd>&c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_i;
      &c_v;&c_y;&c_p;&c_o;&c_l;&c_n;&c_je;&c_n;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">dynamick&yacute;</trd>
  <trd lng="uk">&c_ch;&c_a;&c_s;&c_u; &c_v;&c_i;&#x43A;&#x43E;&#x43D;&c_a;&#x43D;&#x43D;&#x44F;</trd>
  <trdgrp lng="zh">
