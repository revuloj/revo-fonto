<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: devoni.xml,v 1.3 2022/03/15 15:50:55 revo Exp $">
<kap>
  <rad>devoni</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="devoni.0o">
  <kap><tld/>o</kap>
  <snc mrk="devoni.0o.GEOL">
    <uzo tip="fak">MIN</uzo>
    <dif>
      <ref tip="dif" cel="devon.0o">Devono</ref>:
      <ekz>
        anta&ubreve; 374 milionoj da jaroj, en la <tld/>o,
        la Tero konis rapidan kaj grandskalan koloniadon
        de la prakontinento de teraj plantoj sub varma
        kaj tropika klimato
        <fnt>
          <aut>A. Lucas</aut>
          <vrk><url ref="https://teamoisae.wixsite.com/isae/single-post/2018/01/12/La-Devonia-grandskala-estingi%C4%9Do-estas-iomete-plibone-komprenata">
          La Devonia grandskala estingi&gcirc;o estas iomete
          plibone komprenata</url></vrk>
          <lok>ISAE, 2018-01-15</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_v;&c_a;&c_n;</trd>
  <trd lng="ca">devoniano</trd>
  <trd lng="cs">devon</trd>
  <trd lng="de">Devon</trd>
  <trd lng="es">dev&oacute;nico</trd>
  <trd lng="fr">d&eacute;vonien<klr> (subst.)</klr></trd>
  <trd lng="hu">devon</trd>
  <trd lng="nl">devoon</trd>
  <trd lng="pl">Dewon</trd>
  <trd lng="pt">Devoniano</trd>
  <trd lng="ru">&c_d;&c_je;&c_v;&c_o;&c_n;</trd>
  <trd lng="sk">dev&oacute;n</trd>
  <trdgrp lng="uk">
    <trd>&#x434;&#x435;&#x432;&#x43E;&#x43D;</trd>,
    <trd>&#x434;&#x435;&#x432;&#x43E;&#x43D;&#x441;&#x44C;&#x43A;&#x430; &#x441;&#x438;&#x441;&#x442;&#x435;&#x43C;&#x430;</trd>
  </trdgrp>
</drv>
</art>

