<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: oksiko.xml,v 1.22 2024/01/30 16:09:26 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>oksikok</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="oksiko.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="oksiko.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbeto kreskanta en mar&ccirc;oj de malvarmaj landoj,
      kies ru&gcirc;aj beroj estas uzataj por sukoj kaj konfita&jcirc;oj,
      el familio <ref tip="super" cel="erikac.0oj">erikacoj</ref>
      (specioj <trd lng="la">Vaccinium macrocarpon</trd> kaj <trd lng="la">Vaccinium oxycoccos</trd> iafoje rigardataj kiel
      subgenro <trd lng="la">Oxycoccus</trd>):
      <ekz>
        en la proksima lago ekzistas etaj flosantaj insuletoj, kiuj sen&ccirc;ese migradas,
        sur unu kreskas betulo, sur la alia <klr>[&#x2026;]</klr> <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <vrk>Litovio: Ceramika&jcirc;oj</vrk>
        </fnt>;
      </ekz>
      <ekz>
        bongusta <tld/>a likvoro
        <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 19a, p. 111a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="empetr.0o">empetro</ref>,
      <ref cel="vakcin1.0o">vakcinio</ref>
    </refgrp>
  </snc>
    <bld lok="&WCU;/thumb/6/60/VacciniumOxycoccos2.jpg/320px-VacciniumOxycoccos2.jpg" prm="CC BY-SA 3.0">bran&ccirc;eto de <tld/>o kun frukto, Malsupra Saksujo, Germanujo
        <fnt>
          <aut>Christian Fischer, propra verko</aut>
          <lok><url ref="&WCW;/File:VacciniumOxycoccos2.jpg">https://commons.wikimedia.org/wiki/File:VacciniumOxycoccos2.jpg</url></lok>
        </fnt>
    </bld>
  <trd lng="be">&c_zh;&c_u;&c_r;&c_a;&c_v;&c_ib;&c_n;&c_y;</trd>
  <trd lng="cs">klikva bahenn&iacute;</trd>
  <trd lng="de"><ofc>*</ofc>Moosbeere</trd>
  <trd lng="en">cranberry</trd>
  <trdgrp lng="fr">
    <trd>caneberge</trd>,
    <trd>canneberge</trd>
  </trdgrp>
  <trd lng="hu">t&odblac;zeg&aacute;fonya</trd>
  <trd lng="ja">&#x30C4;&#x30EB;&#x30B3;&#x30B1;&#x30E2;&#x30E2;</trd>
  <trd lng="nl">veenbes</trd>
  <trd lng="pl">&zdot;urawina</trd>
  <trd lng="pt">oxicoco</trd>
  <trd lng="ru"><ofc>*</ofc>&c_k;&c_l;&c_ju;&c_k;&c_v;&c_a;</trd>
  <trd lng="sk">k&lcaron;ukva mo&ccaron;iarna</trd>
  <trdgrp lng="uk">
    <trd>&c_zh;&c_u;&c_r;&c_a;&c_v;&c_l;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_k;&c_l;&c_ju;&c_k;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5C0F;&#x7D05;&#x8393; <pr>xi&#x1CE;oh&#xF3;ngm&#xE9;i</pr></trd>,
    <trd>&#x5C0F;&#x7EA2;&#x8393; <pr>xi&#x1CE;oh&#xF3;ngm&#xE9;i</pr></trd>
  </trdgrp>
</drv>
</art>
