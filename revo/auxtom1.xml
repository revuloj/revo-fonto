<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>a&ubreve;tomobil</rad>/o
</kap>
<drv mrk="auxtom1.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">AUT</uzo>
    <ref tip="dif" cel="auxt.0o">a&ubreve;to</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Auto</trd>,
    <trd>Automobil</trd>,
    <trd>Kraftwagen</trd>
  </trdgrp>
  <trd lng="es">automobil</trd>
  <trd lng="fr">automobile</trd>
  <trd lng="hu">szem&eacute;lykocsi</trd>
  <trd lng="nl">auto</trd>
  <trdgrp lng="os">
    <trd>&c_m;&c_a;&c_sh;&c_i;&c_n;&aelig;</trd>,
    <trd>&c_h;&aelig;&c_d;&c_t;&c_u;&c_l;&c_g;&aelig;</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;&c_i;&c_l;&c_mol;</trd>
  <trd lng="sv">bil</trd>
</drv>

<drv mrk="auxtom1.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <ref tip="dif" cel="auxt.0isto">a&ubreve;tisto</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Autofahrer</trd>,
    <trd>Wagenf&uuml;hrer</trd>,
    <trd>Kraftfahrer</trd>
  </trdgrp>
  <trd lng="es">automobilista</trd>
  <trd lng="fr">automobiliste</trd>
  <trd lng="hu">&uacute;rvezet&odblac;</trd>
  <trd lng="nl">automobilist</trd>
  <trd lng="ru">&c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;&c_i;&c_l;&c_i;&c_s;&c_t;</trd>
  <trd lng="sv">bilf&ouml;rare</trd>
</drv>
</art>
