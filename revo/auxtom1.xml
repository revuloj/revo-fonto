<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>a&ubreve;tomobil</rad>/o
</kap>
<drv mrk="auxtom1.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">AUT</uzo>
    <ref tip="dif" cel="auxt.0o">a&ubreve;to.</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Auto</trd>,
    <trd>Automobil</trd>,
    <trd>Kraftwagen</trd>
  </trdgrp>
  <trd lng="fr">automobile</trd>
  <trdgrp lng="os">
    <trd>&c_m;&c_a;&c_sh;&c_i;&c_n;&aelig;</trd>,
    <trd>&c_h;&aelig;&c_d;&c_t;&c_u;&c_l;&c_g;&aelig;</trd>
  </trdgrp>
  
  <trd lng="ru">&#1072;&#1074;&#1090;&#1086;&#1084;&#1086;&#1073;&#1080;&#1083;&#1100;</trd>
  <trd lng="sv">bil</trd>
</drv>
<drv mrk="auxtom1.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <ref tip="dif" cel="auxt.0isto">a&ubreve;tisto.</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Autofahrer</trd>,
    <trd>Wagenf&uuml;hrer</trd>,
    <trd>Kraftfahrer</trd>
  </trdgrp>
  <trd lng="fr">automobiliste</trd>
  <trd lng="ru">&#1072;&#1074;&#1090;&#1086;&#1084;&#1086;&#1073;&#1080;&#1083;&#1080;&#1089;&#1090;</trd>
  <trd lng="sv">bilf&ouml;rare</trd>
</drv>
</art>
