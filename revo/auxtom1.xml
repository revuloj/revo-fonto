<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: auxtom1.xml,v 1.27 2022/04/26 14:57:17 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>a&ubreve;tomobil</rad>/o
</kap>
<drv mrk="auxtom1.0o">
  <kap><tld/>o</kap>
  <snc mrk="auxtom1.0o.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      <ref tip="dif" cel="auxt.0o">A&ubreve;to</ref>:
      <ekz>
        la lasta aventuro dum <tld/>veturo
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        voja&gcirc;antoj, kiuj venas &ccirc;u &scirc;ipe, &ccirc;u bicikle, <tld/>e a&ubreve; piede trovos belajn
        malnovajn monumentojn, bonegan gastronomion kaj komfortajn gastejojn por
        pilgrimantoj
        <fnt>
          <bib>Monato</bib>
          <aut>Rafaela Urue&ntilde;a</aut>
          <vrk><url ref="&Steloj;monato2/006809.html">Hispanio: Vojoj al la sankta urbo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ar ne ekzistas hoteloj en la regno, turisto sen a&ubreve;tomobilo e&ccirc; ne povas
        vesperman&gcirc;i en Seborgo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/006859.html">SEBORGO: Urbeto a&ubreve; sendependa lando?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        se oni posedas domon kaj <tld/>on oni devas forte labori por
        repagi la prunton
        <fnt>
          <aut>Remigiusz Mielcarek</aut>:
          <vrk>Enmigrintoj en ora ka&gcirc;o</vrk>,
          <bib>Monato</bib><lok>jaro 1994a, numero 1a, p. 15a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>akcidento mortigis lin
        <fnt>
          <bib>Monato</bib>
          <aut>Claudinei Ribeiro</aut>
          <vrk><url ref="&Monato;/2003/008125.html">Memore al Gilbert Ledon</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la intenco estis, per plikostigo de karbura&jcirc;o redukti individuan <tld/>trafikon,
        samtempe oni volis doni pli da mono al medikonvena fervojo
        <fnt>
          <bib>Monato</bib>
          <aut>S. Maul</aut>
          <lok><url ref="&Steloj;monato2/007332.html">&Steloj;monato2/007332.html</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_w;&c_t;&c_a;&c_m;&c_a;&c_b;&c_ib;&c_l;&c_mol;
  </trd>
  <trd lng="bo">&#xF55;&#xF7A;&#xF56;&#xF66;&#xF0B;&#xF4A;&#xF7C;&#xF0B;</trd>
  <trd lng="ca">autom&ograve;bil</trd>
  <trdgrp lng="de">
    <trd>Auto</trd>,
    <trd>Automobil</trd>,
    <trd>Kraftwagen</trd>,
    <trd>Kraftfahrzeug</trd>,
    <trd>Kfz</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>car</trd>,
    <trd>automobile</trd>
  </trdgrp>
  <trd lng="es">autom&oacute;vil</trd>
  <trd lng="fr">automobile</trd>
  <trd lng="he">&#x5D0;&#x5D5;&#x5D8;&#x5D5;</trd>
  <trd lng="hu">szem&eacute;lykocsi</trd>
  <trdgrp lng="it">
    <trd>automobile</trd>,
    <trd>vettura<klr> (auto)</klr></trd>, 
    <trd>macchina<klr> (auto)</klr></trd>,
    <trd>auto<klr> (vettura)</klr></trd>,
  </trdgrp>
  <trd lng="nl">auto</trd>
  <trdgrp lng="os">
    <trd>&c_m;&c_a;&c_sh;&c_i;&c_n;&aelig;</trd>,
    <trd>&c_h;&aelig;&c_d;&c_t;&c_u;&c_l;&c_g;&aelig;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>automobil<klr> (przest.)</klr></trd>,
    <trd>auto</trd>,
    <trd>samoch&oacute;d</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>autom&oacute;vel</trd>,
    <trd>carro</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;&c_i;&c_l;&c_mol;
  </trd>
  <trd lng="sv">bil</trd>
  <trd lng="uk">&#x430;&#x432;&#x442;&#x43E;&#x43C;&#x43E;&#x431;&#x456;&#x43B;&#x44C;</trd>
</drv>

<drv mrk="auxtom1.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="auxtom1.0isto.auxtisto">
    <dif>
      <ref tip="dif" cel="auxt.0isto">A&ubreve;tisto</ref>:
      <ekz>
        stirante, <tld/>istoj kaj motorciklistoj telefonas
        <fnt>
          <aut>Christian Fabossou</aut>:
          <vrk>&Ccirc;u-vi-vidas-min-ismo</vrk>, <bib>Monato</bib>
          <lok>jaro 2001a, numero 12a, p. 12a</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u ne estus pli prudente kiel <tld/>isto tute ne trinki alkohola&jcirc;on
        <fnt>
          <bib>Monato</bib>
          <aut>S. Maul</aut>
          <vrk><url ref="&Steloj;monato2/007154.html">Finfine oni povas dibo&ccirc;i sen pento</url></vrk>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">
  &c_a;&c_w;&c_t;&c_a;&c_m;&c_a;&c_b;&c_ib;&c_l;&c_ib;&c_s;&c_t;</trd>
  <trd lng="ca">automobilista</trd>
  <trdgrp lng="de">
    <trd>Autofahrer</trd>,
    <trd>Wagenf&uuml;hrer</trd>,
    <trd>Kraftfahrer</trd>
  </trdgrp>
  <trd lng="en">motorist</trd>
  <trd lng="es">automovilista</trd>
  <trd lng="fr">automobiliste</trd>
  <trd lng="he">&#x5E0;&#x5B6;&#x5D4;&#x5B8;&#x5D2;</trd>
  <trd lng="hu">&uacute;rvezet&odblac;</trd>
  <trdgrp lng="it">
    <trd>automobilista</trd>,
    <trd>autista<klr> (conducente di auto)</klr></trd>
  </trdgrp>
  <trd lng="nl">automobilist</trd>
  <trdgrp lng="pl">
    <trd>automobilista<klr> (przest.)</klr></trd>,
    <trd>kierowca</trd>
  </trdgrp>
  <trd lng="ru">
  &c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;&c_i;&c_l;&c_i;&c_s;&c_t;</trd>
  <trd lng="sv">bilf&ouml;rare</trd>
</drv>
</art>
