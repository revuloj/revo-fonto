<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Mediterane</rad>/o
</kap>
<drv mrk="mediterane.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Maro, loki&gcirc;anta de 30&ring;15' &gcirc;is 45&ring;50' de norda latitudo
      kaj de 6&ring; de okcidenta longitudo &gcirc;is 36&ring; de orienta
      longitudo. &Gcirc;iaj bordoj apartenas al tri
      <ref tip="vid" cel="mond.0oparto">mondopartoj</ref>:
      <ref tip="vid" cel="euxrop1.E0o">E&ubreve;ropo</ref>,
      <ref tip="vid" cel="azi.A0o">Azio</ref> kaj
      <ref tip="vid" cel="afrik.A0o">Afriko</ref>:
      <ekz>
        Edwin de Kock voja&gcirc;is en 1985 al E&ubreve;ropo, al
        <tld/>o<fnt><aut>Franz-Georg R&ouml;ssler</aut>: <vrk>Profite voja&gcirc;u
        kun Edwin</vrk>, <bib>Monato</bib><lok>jaro 1994a, numero 6a, p.
        21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Mittelmeer</trd>
  <trd lng="en">Mediterranean Sea</trd>
  <trdgrp lng="fr">
    <trd>mer <ind>m&eacute;dit&eacute;rran&eacute;e</ind></trd>
  </trdgrp>
  <trd lng="hu">F&ouml;ldk&ouml;zi-tenger</trd>
  <trd lng="nl">Middellandse Zee</trd>
  <trd lng="ru">&c_S;&c_r;&c_je;&c_d;&c_i;&c_z;&c_je;&c_m;&c_n;&c_o;&c_je; &c_m;&c_o;&c_r;&c_je;</trd>
  <trd lng="sk">Stredozemn&eacute; more</trd>
</drv>

<drv mrk="mediterane.0a">
  <kap><tld lit="m"/>a</kap>
  <snc>
    <dif>
      Rilata a&ubreve; apartenanta al <tld/>o:
      <ekz>
        la <tld/>a insulo Santorin
        <fnt><aut>Evgeni Georgiev</aut>: <vrk>Vulkanoj</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 2a, p. 22a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">mittelmeerisch</trd>
  <trd lng="en">Mediterranean</trd>
  <trdgrp lng="hu">
    <trd>f&ouml;ldk&ouml;zi-tengeri</trd>,
    <trd>mediterr&aacute;n</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>m&eacute;diterran&eacute;en</trd>
  </trdgrp>
  <trd lng="nl">mediterraan</trd>
  <trd lng="ru">&c_s;&c_r;&c_je;&c_d;&c_i;&c_z;&c_je;&c_m;&c_n;&c_o;&c_m;&c_o;&c_r;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
