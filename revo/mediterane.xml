<vortaro>
<art mrk="$Id: mediterane.xml,v 1.29 2024/01/25 20:10:35 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>Mediterane</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="mediterane.M0o">
  <kap><tld/>o</kap>
  <snc mrk="mediterane.M0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" lst="voko:maroj" cel="mar.0o">Maro</ref>, loki&gcirc;anta de 30&#x2DA;15' &gcirc;is
      45&#x2DA;50' de
      norda latitudo kaj de 6&#x2DA; de okcidenta longitudo &gcirc;is 36&#x2DA; de
      orienta longitudo. &Gcirc;iaj bordoj apartenas al tri
      <ref tip="vid" cel="mond.0oparto">mondopartoj</ref>:
      <ref tip="vid" cel="euxrop1.E0o">E&ubreve;ropo</ref>,
      <ref tip="vid" cel="azi.A0o">Azio</ref> kaj
      <ref tip="vid" cel="afrik.A0o">Afriko</ref>:
      <ekz>
        la insuloj de <tld/>o
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 24a</lok></fnt>;
      </ekz>
      <ekz>
        <nom>Edwin de Kock</nom> voja&gcirc;is en 1985 al E&ubreve;ropo, al
        <tld/>o
        <fnt>
        <aut>Franz-Georg R&ouml;ssler</aut>,
        <vrk>Profite voja&gcirc;u kun Edwin</vrk>,
        <bib>Monato</bib>, <lok>jaro 1994a, numero 6a, p. 21a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_M;&c_ib;&c_zh;&c_z;&c_je;&c_m;&c_n;&c_a;&c_je;
  &c_m;&c_o;&c_r;&c_a;</trd>
  <trd lng="br">Mor <ind>Kreizdouar</ind></trd>
  <trd lng="ca">mar <klr>Mediterr&agrave;nia</klr></trd>
  <trd lng="cs">St&rcaron;edozemn&iacute; mo&rcaron;e</trd>
  <trd lng="de">Mittelmeer</trd>
  <trd lng="en">Mediterranean Sea</trd>
  <trdgrp lng="es">
    <trd>mar Mediterr&aacute;neo</trd>,
    <trd>Mediterr&aacute;neo</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_d;&a_r;&f_y;&a_A;&f_y;
    <ind>&a_m;&a_d;&f_y;&a_t;&a_r;&a_A;&a_n;&a_h;</ind></trd>,
    <trd>&a_d;&a_r;&f_y;&a_A;&f_y; &a_m;&f_y;&a_A;&a_n;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">mer <ind>M&eacute;diterran&eacute;e</ind></trd>
  <trd lng="hu">F&ouml;ldk&ouml;zi-tenger</trd>
  <trd lng="ja">&#x5730;&#x4E2D;&#x6D77;  <pr>&#x3061;&#x3061;&#x3085;&#x3046;&#x304B;&#x3044;</pr></trd>
  <trd lng="nl">Middellandse Zee</trd>
  <trd lng="pl">Morze &Sacute;r&oacute;dziemne</trd>
  <trd lng="pt">Mediterr&acirc;neo</trd>
  <trd lng="ro">Marea Mediteran&abreve;</trd>
  <trd lng="ru">&c_S;&c_r;&c_je;&c_d;&c_i;&c_z;&c_je;&c_m;&c_n;&c_o;&c_je;
  &c_m;&c_o;&c_r;&c_je;</trd>
  <trd lng="sk">Stredozemn&eacute; more</trd>
  <trd lng="uk">&c_S;&c_je;&c_r;&c_je;&c_d;&c_z;&c_je;&#x43C;&#x43D;&c_je; &#x43C;&#x43E;&c_r;&c_je;</trd>
  <trd lng="zh">&#x5730;&#x4E2D;&#x6D77; <pr>D&#xEC;zh&#x14D;ngH&#x1CE;i</pr></trd>
</drv>

<drv mrk="mediterane.0a">
  <kap><tld lit="m"/>a</kap>
  <snc mrk="mediterane.0a.GEOG">
    <dif>
      Rilata a&ubreve; apartenanta al <tld/>o:
      <ekz>
        la <tld/>a insulo Santorin
        <fnt>
        <aut>Evgeni Georgiev</aut>,
        <vrk>Vulkanoj</vrk>,
        <bib>Monato</bib>, <lok>jaro 2001a, numero 2a, p. 22a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_zh;&c_z;&c_je;&c_m;&c_n;&c_a;&c_m;&c_o;&c_r;&c_s;&c_k;&c_ib;</trd>
  <trd lng="br">kreizdouarel</trd>
  <trd lng="ca">mediterrani</trd>
  <trdgrp lng="de">
    <trd>Mittelmeer-</trd>,
    <trd>mediterran</trd>
  </trdgrp>
  <trd lng="en">Mediterranean</trd>
  <trdgrp lng="es">
    <trd>mediterr&aacute;neo</trd>,
    <trd>mediterr&aacute;nea</trd>
  </trdgrp>
  <trd lng="fa">&a_m;&a_d;&f_y;&a_t;&a_r;&a_A;&a_n;&a_h;&#x200C;&a_A;&f_y;</trd>
  <trd lng="fr">m&eacute;diterran&eacute;en</trd>
  <trdgrp lng="hu">
    <trd>f&ouml;ldk&ouml;zi-tengeri</trd>,
    <trd>mediterr&aacute;n</trd>
  </trdgrp>
  <trd lng="ja">&#x5730;&#x4E2D;&#x6D77;&#x306E;  <pr>&#x3061;&#x3061;&#x3085;&#x3046;&#x304B;&#x3044;&#x306E;</pr></trd>
  <trd lng="nl">mediterraan</trd>
  <trd lng="pl">&sacute;r&oacute;dziemnomorski</trd>
  <trd lng="pt">mediterr&acirc;neo</trd>
  <trd lng="ru">&c_s;&c_r;&c_je;&c_d;&c_i;&c_z;&c_je;&c_m;&c_n;&c_o;&c_m;&c_o;&c_r;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
