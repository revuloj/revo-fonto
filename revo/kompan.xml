<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>1</ofc>
    <rad>kompani</rad>/o
</kap>
<drv mrk="kompan.0o">
    <kap><tld/>o</kap>
    <snc num="1">
      <uzo tip="fak">JUR</uzo>
      <uzo tip="fak">EKON</uzo>
      <dif>
        Kuni&gcirc;o da personoj kune direktantaj komercan a&ubreve;
industrian aferon:
        <ekz>firmo X. kaj <tld/>o;</ekz>
        <ekz>fari <tld/>on kun iu</ekz>
      </dif>
      <refgrp tip="sin">
        <ref cel="asoci.0o">asocio</ref>.
      </refgrp>
      <trd lng="de">Gesellschaft</trd>
      <trdgrp lng="nl">
        <trd>maatschappij</trd>,
        <trd>vennootschap</trd>
      </trdgrp>
    </snc>
    <snc num="2">
      <uzo tip="fak">JUR</uzo>
      <uzo tip="fak">EKON</uzo>
      <dif>
        Tiu komerca a&ubreve; industria afero mem:
        <ekz>akcia <tld/>o;</ekz>
        <ekz>fervoja, asekura <tld/>o</ekz>
      </dif>
      <refgrp tip="sin">
        <ref cel="societ.0o">societo</ref>.
      </refgrp>
      <trd lng="de">Gesellschaft</trd>
      <trdgrp lng="nl">
        <trd>maatschappij</trd>,
        <trd>vennootschap</trd>
      </trdgrp>
</snc>
    <snc num="3">
      <dif>
        <ref tip="dif" cel="rot.0o.2">Roto 2</ref>.
      </dif>
      <trd lng="de">Rotte</trd>
    </snc>
    <snc num="4">
      <dif>
        <ref tip="dif" cel="akompa.0i.1">Akompanado 1</ref>:
        <ekz>
          &scirc;i iris kun sia malgranda infano en <tld/>o de
          du &ccirc;am&ccirc;asistoj<fnt>Z</fnt>;
        </ekz>
        <ekz>en <tld/>o e&ccirc; morto
facili&gcirc;as<fnt><bib>PrV</bib></fnt>;</ekz>
        <ekz>neniam anta&ubreve;e li estis en tia needukita
<tld/>o<fnt>Z</fnt>;</ekz>
        <ekz>tute ne estas agrable havi <tld/>on kun vi<fnt>Z</fnt>.</ekz>
      </dif>
      <trd lng="de">Begleitung</trd>
    <trd lng="nl">gezelschap</trd>
    </snc>
    <trd lng="be">&c_k;&c_a;&c_m;&c_p;&c_a;&c_n;&c_ib;&c_ja;</trd>
    <trd lng="en">company</trd>
    <trd lng="es">compa&ntilde;&iacute;a</trd>
    <trd lng="he">&het;&bet;&resh;&he;</trd>
    <trdgrp lng="fr">
      <trd>compagnie</trd>,
      <trd>soci&eacute;t&eacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_m;&c_p;&c_a;&c_n;&c_i;&c_ja;</trd>,
      <trd>&c_o;&c_b;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
    </trdgrp>
</drv>
<drv mrk="kompan.0ano">
    <kap><tld/>ano<fnt>Z</fnt></kap>
    <snc>
      <uzo tip="fak">JUR</uzo>
      <dif>
        Asociano.
      </dif>
    </snc>
    <trd lng="be">&c_k;&c_a;&c_m;&c_p;&c_a;&c_n;&c_mol;&c_jo;&c_n;</trd>
    <trd lng="de">Gesellschafter</trd>
    <trd lng="en">associate</trd>
    <trd lng="fr">associ&eacute;</trd>
    <trd lng="he">&shin;&vav;&tav;&fpe;</trd>
    <trd lng="nl">vennoot</trd>
    <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_a;&c_n;&c_mol;&c_o;&c_n;</trd>
</drv>
<drv mrk="kompan.0ulo">
    <kap><tld/>ulo<fnt>Z</fnt></kap>
    <snc>
      <dif>
        Akompananto:
        <ekz>lia festena <tld/>ulo, kiu <tld/>is al li<fnt>Z</fnt>.</ekz>
      </dif>
    </snc>
    <trd lng="be">&c_k;&c_a;&c_m;&c_p;&c_a;&c_n;&c_mol;&c_jo;&c_n;</trd>
    <trd lng="de">Begleiter</trd>
    <trd lng="en">companion</trd>
    <trd lng="es">acompa&ntilde;ante</trd>
    <trd lng="fr">compagnon</trd>
    <trdgrp lng="he">
      <trd>&mem;&lamed;&vav;&vav;&he;</trd>,
      <trd>&het;&bet;&resh;</trd>
    </trdgrp>
    <trd lng="nl">metgezel</trd>
    <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_a;&c_n;&c_mol;&c_o;&c_n;</trd>
</drv>
</art>
