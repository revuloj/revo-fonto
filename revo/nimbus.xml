<art mrk="$Id$">
<kap>
  <rad>nimbus</rad>/o
</kap>
<drv mrk="nimbus.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      Pluvoporta griza nubo.
    </dif>
    <refgrp tip="vid">
      <ref cel="kumulu.0o">kumuluso</ref>.
