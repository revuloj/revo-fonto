<vortaro>
<art mrk="$Id: nimbus.xml,v 1.24 2024/07/29 06:33:48 revo Exp $">
<kap>
  <rad>nimbus</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="nimbus.0o">
  <kap><tld/>o</kap>
  <snc mrk="nimbus.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Pluvoporta griza <ref tip="super" cel="nub.0o.MET">nubo</ref>:
      <ekz>
        la nuboj estas nomitaj la&ubreve; sia formo:
        cirusojn, stratusojn, <tld/>ojn a&ubreve; kumulusojn
        <fnt>
          <aut>M. Rollet de l'Isle</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/MaristaTerminaro/MaristaTerminaro.html">
          Provo de Marista Terminaro</url></vrk>,
          <lok>1908</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_zh;&c_d;&c_zh;&c_a;&c_v;&c_a;&c_ja; <ind>&c_h;&c_m;&c_a;&c_r;&c_a;</ind></trd>
  <trdgrp lng="bg">
    <trd>&c_n;&c_i;&c_m;&c_b;&c_u;&c_s;</trd>,
    <trd>&c_d;&c_malmol;&c_zh;&c_d;&c_o;&c_v;&c_je;&c_n; <ind>&c_o;&c_b;&c_l;&c_a;&c_k;</ind></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>nimbus</trd>,
    <trd>nimbe</trd>
  </trdgrp>
  <trd lng="cs">nimbus</trd>
  <trdgrp lng="de">
    <trd>Nimbus</trd>,
    <trd>Regenwolke</trd>,
    <trd>Schleierwolke</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>nimbus</trd>,
    <trd>rain <ind>cloud</ind></trd>
  </trdgrp>
  <trd lng="es">nimbo</trd>
  <trd lng="fa">&a_n;&f_y;&a_m;&a_b;&a_w;&a_s;</trd>
  <trd lng="fr">nimbus</trd>
  <trdgrp lng="hu">
    <trd>es&odblac;felh&odblac;</trd>,
    <trd>nimbusz(felh&odblac;)</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#20081;&#38642;  <pr>&#12425;&#12435;&#12358;&#12435;</pr></trd>,
    <trd>&#38632;&#38642;  <pr>&#12354;&#12414;&#12368;&#12418;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>nimbuswolk</trd>,
    <trd>regenwolk</trd>
  </trdgrp>
  <trdgrp lng="pl">
  <trd>chmura deszczowa</trd>,
  <trd>nimbus</trd>
</trdgrp>
  <trd lng="pt">nimbo</trd>
  <trd lng="ru">&c_d;&c_o;&c_zh;&c_d;&c_je;&c_v;&c_a;&c_ja; <ind>&c_t;&c_u;&c_ch;&c_a;</ind></trd>
  <trd lng="sk">nimbus <klr>(mrak)</klr></trd>
  <trd lng="uk">&c_d;&c_o;&c_shch;&c_o;&c_v;&c_a; (&c_a;&c_b;&c_o; &c_s;&c_n;&c_ib;&c_g;&c_o;&c_v;&c_a;) &c_h;&c_m;&c_a;&c_r;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#31215;&#38632;&#20113; <pr>j&imacron;y&#468;y&uacute;n</pr></trd>,
    <trd>&#31309;&#38632;&#38642; <pr>j&imacron;y&#468;y&uacute;n</pr></trd>,
    <trd>&#38632;&#20113; <pr>y&#468;y&uacute;n</pr></trd>,
    <trd>&#38632;&#38642; <pr>y&#468;y&uacute;n</pr></trd>
  </trdgrp>
</drv>
</art>
