<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>nimbus</rad>/o
</kap>
<drv mrk="nimbus.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      Pluvoporta griza <ref tip="super" cel="nub.0o.MET">nubo</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Nimbus</trd>,
    <trd>Regenwolke</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>nimbus</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>nimbus</trd>,
    <trd>rain <ind>cloud</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>nimbuswolk</trd>,
    <trd>regenwolk</trd>
  </trdgrp>
  <trd lng="pt">nimbo</trd>
  <trd lng="ru">&c_d;&c_o;&c_zh;&c_d;&c_je;&c_v;&c_a;&c_ja; <ind>&c_t;&c_u;&c_ch;&c_a;</ind></trd>
</drv>
</art>
