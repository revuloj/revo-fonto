  </snc>
  <trdgrp lng="de">
    <trd>Nimbus</trd>,
    <trd>Regenwolke</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>nimbuswolk</trd>,
    <trd>regenwolk</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_o;&c_zh;&c_d;&c_je;&c_v;&c_a;&c_ja; <ind>&c_t;&c_u;&c_ch;&c_a;</ind></trd>
</drv>
</art>
