<kap>
  <rad>asiz</rad>/o
</kap>
<drv mrk="asiz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Kunsido de kriminala tribunalo.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_d; &c_p;&c_r;&c_y;&c_s;&c_ja;&c_zh;&c_n;&c_y;&c_h;</trd>
  <trdgrp lng="cs">
    <trd>porota</trd>,
    <trd>porotn&iacute; soud</trd>,
    <trd>p&rcaron;&iacute;se&zcaron;n&yacute; soud</trd>
  </trdgrp>
  <trd lng="de">Schwurgericht</trd>
  <trd lng="en">assize court</trd>
  <trd lng="fr">cour d'assises (session)</trd>
  <trd lng="he">&#x5D9;&#x5E9;&#x5D9;&#x5D1;&#x5EA; &#x5D1;&#x5D9;&#x5EA; &#x5D3;&#x5D9;&#x5DF;
  &#x5E4;&#x5DC;&#x5D9;&#x5DC;&#x5D9;</trd>
  <trd lng="hu">esk&uuml;dtsz&eacute;ki t&aacute;rgyal&aacute;s</trd>
  <trd lng="it">assise</trd>
  <trdgrp lng="ja">
    <trd>&#x91CD;&#x7F6A;&#x88C1;&#x5224;  <pr>&#x3058;&#x3085;&#x3046;&#x3056;&#x3044;&#x3055;&#x3044;&#x3070;&#x3093;</pr></trd>,
    <trd>&#x5DE1;&#x56DE;&#x88C1;&#x5224;  <pr>&#x3058;&#x3085;&#x3093;&#x304B;&#x3044;&#x3055;&#x3044;&#x3070;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">zitting v.h. <ind>assisenhof</ind></trd>
  <trdgrp lng="pl">
    <trd>posiedzenie <ind>s&aogonek;du przysi&eogonek;g&lstroke;ych</ind></trd>,
    <trd>assyza</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_u;&c_d; &c_p;&c_r;&c_i;&c_s;&c_ja;&c_zh;&c_n;&c_y;&c_h;</trd>
  <trd lng="sk">zasadanie poroty</trd>
</drv>

<drv mrk="asiz.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      De <tld/>o:
      <ekz>
        <tld/>a tribunalo ne estas konstanta, &gcirc;i estas
        la&ubreve;bezone kunvokata.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_s;&c_ja;&c_zh;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_u;&c_d;&c_a; &c_p;&c_r;&c_y;&c_s;&c_ja;&c_zh;&c_n;&c_y;&c_h;</trd>
  </trdgrp>
  <trd lng="hu">esk&uuml;dtsz&eacute;ki</trd>
  <trd lng="it">d'<ind>assise</ind></trd>
  <trd lng="nl">assisen-</trd>
  <trd lng="de">schwurgerichts-</trd>
</drv>
</art>
