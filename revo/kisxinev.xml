<vortaro>
  <art mrk="$Id: kisxinev.xml,v 1.6 2023/12/03 09:58:49 afido Exp $">
    <kap>
      <rad>Ki&scirc;inev</rad>/o
    </kap>

<drv mrk="kisxinev.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc mrk="kisxinev.0o.urbo">
    <dif>
      La <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&ccirc;efurbo</ref>
      de <ref tip="malprt" cel="moldav.M0ujo.GEOG">Moldavujo</ref>:
      <ekz>
        voja&gcirc;o al la moldava &ccirc;efurbo <tld/>o
        <fnt>
          <vrk><url ref="https://eo.globalvoices.org/2020/05/8642/">Kion signifas la senpopoli&gcirc;o de
          Moldavio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la &Scirc;tata Cirko de <tld/>o
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Cirko_%C5%9Dtata_de_Ki%C5%9Dinevo">Cirko &Scirc;tata de
          Ki&scirc;inevo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en <tld/>o giganta financa fra&ubreve;do pelis la lo&gcirc;antaron sur la straton
        <fnt>
          <aut>Julia Beurq</aut>
          <vrk><url ref="&Mondediplo;article2417.html">Post la &leftquot;romp&scirc;telo de la jarcento&rightquot; en
          Moldavio</url></vrk>
          <lok>Le Monde diplomatique' en Esperanto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_K;&c_ib;&c_sh;&c_y;&c_n;&c_jo;&c_w;</trd>,
      <trd>&c_K;&c_ib;&c_sh;&c_y;&c_n;&c_e;&c_w;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Kischinau</trd>,
      <trd>Chi&#x219;in&abreve;u</trd>
    </trdgrp>
    <trd lng="en">Chi&#x219;in&abreve;u</trd>
    <trd lng="fr">Chi&#x219;in&abreve;u</trd>
    <trd lng="mo">Chi&#x219;in&abreve;u</trd>
    <trd lng="zh">&#x57FA;&#x897F;&#x7D10; <pr>J&#x12B;x&#x12B;ni&#x1D4;</pr></trd>
</snc>
  <trd lng="ja">&#x30AD;&#x30B7;&#x30CB;&#x30E7;&#x30D5;</trd>
  <trd lng="uk">&#x41A;&c_i;&c_sh;&c_i;&#x43D;&c_ib;&c_v;</trd>
</drv>

