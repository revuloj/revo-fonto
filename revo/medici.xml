<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: medici.xml,v 1.37 2024/07/29 06:33:49 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>medicin</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="medici.0o">
  <kap><tld/>o</kap>
  <snc mrk="medici.0o.SCI">
    <uzo tip="fak">MED</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="lst" cel="scienc.0o.fako" lst="voko:sciencoj">Scienco</ref> pri sano kaj malsano,
      aplikata al <ref cel="kurac.0o">kuracado</ref>:
      <ekz>
        oni instruis la <tld/>on
        <fnt>
          <bib>Far3</bib>
          <lok>&ccirc;apitro 9a</lok>
        </fnt>;
      </ekz>
      <ekz>
        konscience li studadis <tld/>on
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        estas nepardonebla, ke tiu homo ne studis la <tld/>on, li certe
        elpensus novan
        pulvoron kontra&ubreve; la kropo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>akto unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        fakultato pri <tld/>o
        <fnt><aut>B. Selimi</aut> <vrk>Voja&gcirc;impresoj, somero
        2000</vrk> <bib>Monato</bib>, <lok>2001:1, p.
        26a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="art.kurac0o">kuracarto</ref>
  </snc>
  <snc mrk="medici.0o.medicinajxo">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="medici.0ajxo">Medicina&jcirc;o</ref>, kuracilo:
      <ekz>
        Kristoforo devis rapide kuri al la apoteko por alporti <tld/>on
        <fnt>
          <bib>HsT</bib>
          <lok>Feli&ccirc;a homo</lok>
        </fnt>.
      </ekz>
    </dif>
    <!-- ne traduku EVI -->
  </snc>
  <trd lng="be">&c_m;&c_e;&c_d;&c_y;&c_c;&c_y;&c_n;&c_a;</trd>
  <trdgrp lng="br">
    <trd>medisinerezh</trd>,
    <trd>mezegiezh</trd>
  </trdgrp>
  <trd lng="ca">medicina</trd>
  <trdgrp lng="cs">
    <trd>l&eacute;ka&rcaron;stv&iacute;</trd>,
    <trd>medic&iacute;na</trd>
  </trdgrp>
  <trd lng="de">Medizin</trd>
  <trd lng="en">medicine</trd>
  <trd lng="es">medicina</trd>
  <trdgrp lng="fr">
    <trd>m&eacute;decine</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>orvostan</trd>,
    <trd>gy&oacute;gy&aacute;szat</trd>,
    <trd>medic&iacute;na</trd>
  </trdgrp>
  <trd lng="id"><baz>dokter</baz>kedokteran</trd>
  <trdgrp lng="ja">
    <trd>&#21307;&#23398;  <pr>&#12356;&#12364;&#12367;</pr></trd>,
    <trd>&#21307;&#34899;  <pr>&#12356;&#12376;&#12421;&#12388;</pr></trd>,
    <trd>&#21307;&#30274;  <pr>&#12356;&#12426;&#12423;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">geneeskunde</trd>
  <trd lng="pl">medycyna</trd>
  <trd lng="pt">medicina</trd>
  <trd lng="ro">medicin&abreve;</trd>
  <trd lng="ru">&c_m;&c_je;&c_d;&c_i;&c_c;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">lek&aacute;rska veda</trd>
  <trd lng="uk">&c_m;&c_je;&c_d;&c_i;&c_c;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#21307;&#23398; <pr>y&imacron;xu&eacute;</pr></trd>,
    <trd>&#37291;&#23416; <pr>y&imacron;xu&eacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="medici.0a">
  <kap><tld/>a</kap>
  <snc mrk="medici.0a.SCI">
    <dif>
      Rilata al <tld/>o, uzata en <tld/>o:
      <ekz>
        viziti la <tld/>an lernejon
        <fnt>
          <bib>Far2</bib>
          <lok>&ccirc;apitro 1a</lok>
        </fnt>;
      </ekz>
      <ekz>
        industria kaj <tld/>a uzo de ceramika&jcirc;oj
        <fnt>
          <aut>B. Golden</aut> <vrk>&Ccirc;ina ceramika historio (Zhao
          Hongsheng)</vrk> <bib>Monato</bib> <lok>1995:8,
          p. 22a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_d;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="br">
    <trd>medisinel</trd>,
    <trd>mezegel</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>m&egrave;dic</trd>,
    <trd>medicinal</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>medic&iacute;nsk&yacute;</trd>,
    <trd>zdravotn&iacute;</trd>
  </trdgrp>
  <trd lng="de">medizinisch</trd>
  <trd lng="en">medical</trd>
  <trdgrp lng="es">
    <trd>m&eacute;dica</trd>,
    <trd>medicinal</trd>
  </trdgrp>
  <trd lng="fr">m&eacute;dical</trd>
  <trdgrp lng="hu">
    <trd>orvostani</trd>,
    <trd>gy&oacute;gy&aacute;szati</trd>
  </trdgrp>
  <trd lng="id">medis</trd>
  <trdgrp lng="ja">
    <trd>&#21307;&#23398;&#12398;  <pr>&#12356;&#12364;&#12367;&#12398;</pr></trd>,
    <trd>&#21307;&#23398;&#30340;&#12394;  <pr>&#12356;&#12364;&#12367;&#12390;&#12365;&#12394;</pr></trd>,
    <trd>&#34220;&#29992;&#12398;  <pr>&#12420;&#12367;&#12424;&#12358;&#12398;</pr></trd>
  </trdgrp>
  <trd lng="nl">geneeskundig</trd>
  <trd lng="pl">medyczny</trd>
  <trd lng="ro">medical </trd>
  <trd lng="ru">&c_m;&c_je;&c_d;&c_i;&c_c;&c_i;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>medic&iacute;nsky</trd>,
    <trd>zdravotn&yacute;</trd>
  </trdgrp>
  <trd lng="uk">&c_m;&c_je;&c_d;&c_i;&c_ch;&c_n;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#21307;&#25252; <pr>y&imacron;h&ugrave;</pr></trd>,
    <trd>&#37291;&#35703; <pr>y&imacron;h&ugrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="medici.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="medici.0ajxo.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="dif" cel="kurac.0ilo">Kuracilo</ref>:
      <ekz>
        la vino havis ian aldonan guston, &gcirc;i estis vera <tld/>a&jcirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen aperis <tld/>a&jcirc;o <klr>[&#8230;]</klr> la plej maldol&ccirc;a, kiun oni
        povas trovi
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a&jcirc;oj, anta&ubreve;e senpage ricevataj, i&gcirc;is tro
        multekostaj
        <fnt><aut>A. Gon&ccirc;arov</aut> <vrk>Via sano en viaj
          manoj</vrk>
          <bib>Monato</bib> <lok>2002:3, p.
          17-19a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_e;&c_d;&c_y;&c_k;&c_a;&c_m;&c_e;&c_n;&c_t;</trd>,
    <trd>&c_l;&c_je;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="br">louzaouadur</trd>
  <trdgrp lng="ca">
    <trd>medecina</trd>,
    <trd>medicament</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Medikament</trd>,
    <trd>Medizin <klr>(Arznei)</klr></trd>
  </trdgrp>
  <trd lng="en">medicine</trd>
  <trdgrp lng="es">
    <trd>medicina</trd>,
    <trd>medicamento</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>traitement<klr> (m&eacute;decine)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gy&oacute;gyszer</trd>,
    <trd>medic&iacute;na</trd>
  </trdgrp>
  <trd lng="id">obat</trd>
  <trdgrp lng="nl">
    <trd>geneesmiddel</trd>,
    <trd>medicijn</trd>
  </trdgrp>
  <trd lng="pl">lek</trd>
  <trd lng="ro">medicament</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_je;&c_k;&c_a;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_m;&c_je;&c_d;&c_i;&c_k;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
  <trd lng="uk">&c_m;&c_je;&c_d;&c_i;&c_k;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#21307; <pr>y&imacron;</pr></trd>,
    <trd>&#37291; <pr>y&imacron;</pr></trd>,
    <trd>&#21307;&#23398; <pr>y&imacron;xu&eacute;</pr></trd>,
    <trd>&#37291;&#23416; <pr>y&imacron;xu&eacute;</pr></trd>,
    <trd>&#33647;&#29289; <pr>y&agrave;ow&ugrave;</pr></trd>,
    <trd>&#34277;&#29289; <pr>y&agrave;ow&ugrave;</pr></trd>,
    <trd>&#33647;&#21697; <pr>y&agrave;op&#464;n</pr></trd>,
    <trd>&#34277;&#21697; <pr>y&agrave;op&#464;n</pr></trd>,
    <trd>&#33647; <pr>y&agrave;o</pr></trd>,
    <trd>&#34277; <pr>y&agrave;o</pr></trd>,
    <trd>&#21307;&#33647; <pr>y&imacron;y&agrave;o</pr></trd>,
    <trd>&#37291;&#34277; <pr>y&imacron;y&agrave;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="medici.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="medici.0isto.SCI">
    <uzo tip="fak">MED</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Fakulo pri <tld/>o:
      <ekz>
        filozofoj kaj <tld/>istoj instruas nin, kiel bonege la humora
        stato de la
        spirito akordi&gcirc;as kun la movoj de la materio
        <fnt>
          <bib>Rabistoj</bib>
          <lok>akto dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        diskutis 9000 <tld/>istoj dum la 10a E&ubreve;ropa
        Kancerkongreso
        <fnt><aut>E. Georgiev</aut> <vrk>Novaj kuracmetodoj kaj
        prevento</vrk>, <bib>Monato</bib>, <lok>2001:4,
        p. 22a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_d;&c_y;&c_k;</trd>
  <trdgrp lng="br">
    <trd>medisin</trd>,
    <trd>mezeg</trd>
  </trdgrp>
  <trd lng="ca">metge</trd>
  <trd lng="cs">l&eacute;ka&rcaron;</trd>
  <trdgrp lng="de">
    <trd>Mediziner</trd>,
    <trd>Arzt</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>physician</trd>,
    <trd>doctor</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>doctor<klr> (medicina)</klr></trd>,
    <trd>m&eacute;dico</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>docteur<klr> (m&eacute;decin)</klr></trd>,
    <trd>m&eacute;decin</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>orvos</trd>,
    <trd>gy&oacute;gy&aacute;sz</trd>
  </trdgrp>
  <trd lng="id">dokter</trd>
  <trdgrp lng="ja">
    <trd>&#21307;&#23398;&#32773;  <pr>&#12356;&#12364;&#12367;&#12375;&#12419;</pr></trd>,
    <trd>&#21307;&#24107;  <pr>&#12367;&#12377;&#12375;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>arts</trd>,
    <trd>dokter</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>lekarz</trd>
    <trd>medyk</trd>
  </trdgrp>
  <trd lng="ro">medic</trd>
  <trd lng="ru">&c_m;&c_je;&c_d;&c_i;&c_k;</trd>
  <trd lng="sk">lek&aacute;r</trd>
  <trd lng="uk">&c_m;&c_je;&c_d;&c_i;&c_k;</trd>
</drv>

<drv mrk="medici.krim0o">
  <kap>
    krim<tld/>o,
    <var><kap>jur<tld/>o <fnt><bib>PIV1</bib></fnt></kap></var>
  </kap>
  <snc mrk="medici.krim0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Bran&ccirc;o de <tld/>o, kiu helpas justicon per esploroj
      pri sanstato, vundoj, mortoka&ubreve;zoj:
      <ekz>
        krim<tld/>a ekspertizo
        <fnt>
          <aut>N. Tarasova, D. Mustafin, trad. A. Pe&ccirc;&euml;nkin</aut>
          <vrk>Dmitrij Mendelejev: la instruoj de profeto</vrk>
          <lok>Unesko Kuriero, 2019-07 &gcirc;is 09</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_d;&c_o;&c_v;&c_a;&c_ja; <ind>&c_m;&c_e;&c_d;&c_y;&c_c;&c_y;&c_n;&c_a;</ind></trd>
  <trd lng="en">forensic medicine</trd>
  <trd lng="fr">m&eacute;decine l&eacute;gale</trd>
  <trd lng="id"><baz>dokter</baz>kedokteran forensik</trd>
  <trd lng="ja">&#27861;&#21307;&#23398;  <pr>&#12411;&#12358;&#12356;&#12364;&#12367;</pr></trd>
  <trd lng="pl">medycyna s&aogonek;dowa</trd>
  <trd lng="ro">medicin&abreve; legal&abreve;</trd>
  <trd lng="uk">&c_p;&c_r;&c_a;&c_v;&c_o;&c_v;&c_a; &c_m;&c_je;&c_d;&c_i;&c_c;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#27861;&#21307;&#23398; <pr>f&#462;y&imacron;xu&eacute;</pr></trd>,
    <trd>&#27861;&#37291;&#23416; <pr>f&#462;y&imacron;xu&eacute;</pr></trd>
  </trdgrp>
</drv>
</art>
