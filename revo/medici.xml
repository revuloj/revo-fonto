<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>1</ofc>
<rad>medicin</rad>/o</kap>
<drv mrk="medici.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <uzo tip="fak">SCI</uzo>
  <snc>
    <dif>
      Scienco pri sano kaj malsano, aplikata al kuracado:
      <ekz>
        fakultato pri medicino<fnt><aut>Bardhyl Selimi</aut>:
        <vrk>Voja&gcirc;impresoj, somero 2000</vrk>, <bib>Monato</bib><lok>jaro
        2001a, numero 1a, p. 26a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_d;&c_y;&c_c;&c_y;&c_n;&c_a;</trd>
  <trd lng="de">Medizin</trd>
  <trdgrp lng="hu">
    <trd>orvostan</trd>,
    <trd>gy&oacute;gy&aacute;szat</trd>,
    <trd>medic&iacute;na</trd>
  </trdgrp>
  <trd lng="nl">geneeskunde</trd>
  <trdgrp lng="fr">
    <trd>m&eacute;decine</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_je;&c_d;&c_i;&c_c;&c_i;&c_n;&c_a;</trd>
</drv>

<drv mrk="medici.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o, uzata en <tld/>o:
      <ekz>
        industria kaj <tld/>a uzo de ceramika&jcirc;oj<fnt><aut>Bernard
        Golden</aut>: <vrk>&Ccirc;ina ceramika historio (Zhao Hongsheng)</vrk>,
        <bib>Monato</bib><lok>jaro 1995a, numero 8a, p. 22a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_d;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="hu">
    <trd>orvostani</trd>,
    <trd>gy&oacute;gy&aacute;szati</trd>
  </trdgrp>
  <trd lng="nl">geneeskundig</trd>
</drv>

<drv mrk="medici.0ajxo">
  <kap><tld/>a&jcirc;o<fnt>Z</fnt></kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <ref tip="dif" cel="kurac.0ilo">kuracilo</ref>
    <ekz>
      la <tld/>a&jcirc;oj, anta&ubreve;e senpage ricevataj, i&gcirc;is tro
      multekostaj<fnt><aut>Anatolo Gon&ccirc;arov</aut>: <vrk>Via sano en viaj
      manoj</vrk>, <bib>Monato</bib><lok>jaro 2002a, numero 3a, p.
      17-19a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_e;&c_d;&c_y;&c_k;&c_a;&c_m;&c_e;&c_n;&c_t;</trd>,
    <trd>&c_l;&c_je;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="de">Medikament</trd>
  <trdgrp lng="fr">
    <trd>traitement<klr> (m&eacute;decine)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gy&oacute;gyszer</trd>,
    <trd>medic&iacute;na</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>geneesmiddel</trd>,
    <trd>medicijn</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_je;&c_k;&c_a;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_m;&c_je;&c_d;&c_i;&c_k;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="medici.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Fakulo pri <tld/>o:
      <ekz>
        diskutis 9000 <tld/>istoj dum la 10a E&ubreve;ropa
        Kancerkongreso<fnt><aut>Evgeni Georgiev</aut>: <vrk>Novaj
        kuracmetodoj kaj prevento</vrk>, <bib>Monato</bib><lok>jaro 2001a,
        numero 4a, p. 22a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_d;&c_y;&c_k;</trd>
  <trdgrp lng="fr">
    <trd>docteur<klr> (m&eacute;decin)</klr></trd>,
    <trd>m&eacute;decin</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>orvos</trd>,
    <trd>gy&oacute;gy&aacute;sz</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>arts</trd>,
    <trd>dokter</trd>
  </trdgrp>
</drv>
</art>
