<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ekstravaganc</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="ekstra1.0o">
  <kap><tld/>o</kap>
    <fnt><bib>ZR</bib></fnt>
  <snc>
    <dif>
      Ago a&ubreve; parolo kontra&ubreve;a al la komuna sa&gcirc;o,
      sensenca stranga&jcirc;o: 
      <ekz>kelkaj lirikaj poetoj faras el sia arto harmonian
      <tld/>on.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="delir.0o">deliro</ref>,
      <ref cel="frenez">frenezo</ref>,
      <ref cel="petol">petolo</ref>.
    </refgrp>
  </snc>
  <trd
  lng="be">&c_e;&c_k;&c_s;&c_t;&c_r;&c_a;&c_v;&c_a;&c_g;&c_a;&c_n;&c_t;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
     <trd>Extravaganz</trd>,
     <trd>&Uuml;berspanntheit</trd>
  </trdgrp>
  <trd lng="en">extravagance</trd>
  <trd lng="es">extravagancia</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;raison</trd>,
    <trd>excentricit&eacute;<klr tip="ind"> (extravagance)</klr></trd>,
    <trd>extravagance</trd></trdgrp>
  <trdgrp lng="hu">
    <trd>extravagancia</trd>,
    <trd>k&uuml;l&ouml;nck&ouml;d&eacute;s</trd>,
    <trd>h&oacute;bort</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ekstrawagancja</trd>,
    <trd>dziwactwo</trd>
  </trdgrp>
  <trd lng="pt">extravag&acirc;ncia</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_k;&c_s;&c_t;&c_r;&c_a;&c_v;&c_a;&c_g;&c_a;&c_n;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_ch;&c_u;&c_d;&c_a;</trd>
  </trdgrp>
</drv>
<drv mrk="ekstra1.0a">
  <kap><tld/>a</kap>
  <snc num="1">
    <dif>
      Kontra&ubreve;a al la komuna sa&gcirc;o, sensence stranga. 
    </dif>
    <refgrp tip="vid">
      <ref cel="bizar.0a">bizara</ref>,
      <ref cel="frenez.0a">freneza</ref>.
    </refgrp>
  </snc>
  <snc num="2">
    <dif>
      <tld lit="E"/>e aganta, faranta <tld/>ojn. 
    </dif>
  </snc>
  <trd
  lng="be">&c_e;&c_k;&c_s;&c_t;&c_r;&c_a;&c_v;&c_a;&c_g;&c_a;&c_n;&c_t;&c_n;&c_y;</trd>
  <trdgrp lng="de">
     <trd>extravagant</trd>,
     <trd>verstiegen</trd>,
     <trd>&uuml;berspannt</trd>
  </trdgrp>
  <trd lng="en">extravagant</trd>
  <trdgrp lng="fr">
    <trd>excentrique<klr tip="ind"> (extravagant)</klr></trd>,
    <trd>extravagant</trd>,
    <trd>d&eacute;raisonnable</trd></trdgrp> 
  <trdgrp lng="hu">
    <trd>extravag&aacute;ns</trd>,
    <trd>k&uuml;l&ouml;nc</trd>,
    <trd>h&oacute;bortos</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ekstrawagancki</trd>,
    <trd>dziwaczny</trd>,
    <trd>cudaczny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_k;&c_s;&c_t;&c_r;&c_a;&c_v;&c_a;&c_g;&c_a;&c_n;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_ch;&c_u;&c_d;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
<drv mrk="ekstra1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <tld lit="E"/>e konduti.  
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_y;&c_c;&c_mol;
  <ind>&c_e;&c_k;&c_s;&c_t;&c_r;&c_a;&c_v;&c_a;&c_g;&c_a;&c_n;&c_t;&c_n;&c_y;&c_m;</ind></trd>
  <trdgrp lng="de">
     <trd>sich extravagant benehmen</trd>,
     <trd>verstiegen sein</trd>,
     <trd>&uuml;berspannt handeln</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;raisonner</trd>,
    <trd>extravaguer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>extravag&aacute;ns</trd>,
    <trd>k&uuml;l&ouml;nck&ouml;dik</trd>,
    <trd>h&oacute;bortos</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zachowywa&cacute; si&eogonek; <ind>ekstrawagancko</ind></trd>,
    <trd>zachowywa&cacute; si&eogonek; <ind>dziwacznie</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_je;&c_s;&c_t;&c_i; &c_s;&c_je;&c_b;&c_ja;
  &c_e;&c_k;&c_s;&c_t;&c_r;&c_a;&c_v;&c_a;&c_g;&c_a;&c_n;&c_t;&c_n;&c_o;</trd>
</drv>
</art>
