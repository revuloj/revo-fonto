<kap>
  <rad>ekstravaganc</rad>/o <fnt><bib>ZR</bib></fnt>
</kap>
<drv mrk="ekstra1.0o">
  <kap><tld/>o</kap>
  <snc mrk="ekstra1.0o.MOR">
    <dif>
      Ago a&ubreve; parolo kontra&ubreve;a al la komuna <ref tip="ant" cel="sagx.0o.prudento">sa&gcirc;o</ref>,
      <ref tip="super" cel="strang.0ajxo">stranga&jcirc;o</ref>:
      <ekz>
        ili volus per io ekbrili, glori&gcirc;i, okupi plej altan lokon en la socio kaj akiri
        tiamaniere liberecon fari kion ili volas kaj &scirc;irmi siajn <tld/>ojn per
        imagata plialteco
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        kia <tld/>o, renkonti vin kune, ho, mia povra <nom>Liza</nom>, &ccirc;u vi ne povis &ccirc;erpi pli
        indan viron ol tiun <nom>Vik</nom>, kiu iniciatis haremojn en la oriento
        <fnt>
          <bib>KrM</bib>
        </fnt>?
      </ekz>
      <ekz>
        jen iuj urbestroj organizas publikan propagandadon de <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Atanasije Marjanovi&cacute;</aut>
          <vrk><url ref="&Monato;/2013/010942.php">Lasta vorto</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        la epiteto &leftquot;verda&rightquot; kaj similaj <tld/>oj komplete malaperis, dum la uzo de
        &leftquot;artefarita&rightquot; kaj &leftquot;helpa&rightquot;, kvankam reduktita, beda&ubreve;rinde ankora&ubreve; obstinas
        <fnt>
          <bib>EfI</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="luks.0o">lukso</ref>,
      <ref cel="privil.0o">privilegio</ref>,
      <ref cel="snob.0ismo">snobismo</ref>
    </refgrp>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Anka&ubreve; eblas diri <ke>nekomuna stranga&jcirc;o</ke>,
    <ke>kondutstrangeco</ke>.
  </rim>

  <trd lng="be">&c_e;&c_k;&c_s;&c_t;&c_r;&c_a;&c_v;&c_a;&c_g;&c_a;&c_n;&c_t;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="ca">extravag&agrave;ncia</trd>
  <trdgrp lng="cs">
    <trd>extravagance</trd>,
    <trd>extravagantnost</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Extravaganz</trd>,
     <trd>&Uuml;berspanntheit</trd>
  </trdgrp>
  <trd lng="en">extravagance</trd>
  <trd lng="es">extravagancia</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;raison</trd>,
    <trd>excentricit&eacute;<klr tip="ind"> (extravagance)</klr></trd>,
    <trd>extravagance</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>extravagancia</trd>,
    <trd>k&uuml;l&ouml;nck&ouml;d&eacute;s</trd>,
    <trd>h&oacute;bort</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#38750;&#24120;&#35672;&#12394;&#35328;&#34892;  <pr>&#12402;&#12376;&#12423;&#12358;&#12375;&#12365;&#12394;&#12370;&#12435;&#12371;&#12358;</pr></trd>,
    <trd>&#28961;&#33590;&#12394;&#34892;&#28858;  <pr>&#12416;&#12385;&#12419;&#12394;&#12371;&#12358;&#12356;</pr></trd>,
    <trd>&#36884;&#26041;&#12418;&#12394;&#12356;&#32771;&#12360;  <pr>&#12392;&#12411;&#12358;&#12418;&#12394;&#12356;&#12363;&#12435;&#12364;&#12360;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ekstrawagancja</trd>,
    <trd>dziwactwo</trd>
  </trdgrp>
  <trd lng="pt">extravag&acirc;ncia</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_k;&c_s;&c_t;&c_r;&c_a;&c_v;&c_a;&c_g;&c_a;&c_n;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_ch;&c_u;&c_d;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>podiv&iacute;nstvo</trd>,
    <trd>v&yacute;strednos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_je;&c_k;&c_s;&c_t;&c_r;&c_a;&c_v;&c_a;&c_gu;&c_a;&c_n;&c_t;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_je;&c_z;&c_v;&c_i;&c_ch;&c_a;&c_j;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_m;&c_h;&c_l;&c_i;&c_v;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_i;&c_g;&c_a;&c_d;&c_l;&c_i;&c_v;&c_ib;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="zh">&#37610;&#24373; <pr>p&umacron;zh&amacron;ng</pr></trd>
</drv>

<drv mrk="ekstra1.0a">
  <kap><tld/>a</kap>
  <snc mrk="ekstra1.0a.io">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Kontra&ubreve;a al la komuna sa&gcirc;o, sensence
      <ref tip="vid" cel="strang.0a.KOMUNE">stranga</ref>:
      <ekz>
        la avangarda romano Hura!, <klr>[&#8230;]</klr> miksas <klr>[&#8230;]</klr> elementojn de
        filozofieca satiro, gajege <tld/>a farsa satiro, sciencfikcio kaj fantazio
        <fnt>
          <bib>Plumamikoj</bib>
        </fnt>;
      </ekz>
      <ekz>
        periodo, dum kiu oni enkondukis kelkajn <tld/>a&jcirc;ojn kaj gaje eksperimentis pri
        novaj vortoj kaj sufiksoj, estis balda&ubreve; likvidita per decido, en 1925,
        kontra&ubreve; tro facilanima enkonduko de neologismoj
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        lia kreema spirito elpensis tre diversajn <tld/>a&jcirc;ojn <klr>[&#8230;]</klr>, letero,
        skribita de li <klr>[&#8230;]</klr> la vortoj estas prezentataj en iu dialekta
        formo de la nederlanda lingvo, sed krome kun apliko de iu &ndash; supozeble de li
        mem elpensita &ndash; gramatika strukturo
        <fnt>
          <bib>Monato</bib>
          <aut>W. M. A. de Smet</aut>
          <vrk><url ref="&Monato;/2004/008418.html">Edward Croegaert, la forgesita
          lingvokreanto</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kapt.atento0a">atentokapta</ref>,
      <ref cel="bizar.0a">bizara</ref>,
      <ref cel="ekscentr.0a">ekscentra</ref>,
      <ref cel="ordina.ekster0a">eksterordinara</ref>
    </refgrp>
  </snc>
  <snc mrk="ekstra1.0a.iu">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      <tld lit="E"/>e aganta, faranta <tld/>ojn:
      <ekz>
        <klr>[jen]</klr> somerdomo de ia <tld/>a granda sinjoro, <klr>[&#8230;]</klr>
        eksterordinare preciza, perfekta, solida kaj atestanta pri alta kulturo
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ion &scirc;i volus aksigi &ccirc;irka&ubreve; si, &ccirc;iam &scirc;i strebas altiradi atenton al sia <tld/>a
        memo
        <fnt>
          <bib>SkandalJozef</bib>
        </fnt>;
      </ekz>
      <ekz>
        ke temis pri <tld/>ulo, kiel cetere preska&ubreve; &ccirc;iuj artistoj, estas delonge konate
        <klr>[&#8230;]</klr> sed neniu <klr>[&#8230;]</klr> imagis, ke <klr>[li]</klr> tapetos la
        murojn de la urbeto per centoj da larmeltiraj nekrologoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2011/010281.php">Mankas kapro en lia kapo</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekscentr.0a">ekscentra</ref>,
      <ref cel="frenez.0a">freneza</ref>,
      <ref cel="modest.mal0a">malmodesta</ref>,
      <ref cel="pruden.mal0a">malprudenta</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_s;&c_t;&c_r;&c_a;&c_v;&c_a;&c_g;&c_a;&c_n;&c_t;&c_n;&c_y;</trd>
  <trd lng="ca">extravagant</trd>
  <trdgrp lng="cs">
    <trd>extravagantn&iacute;</trd>,
    <trd>v&yacute;st&rcaron;edn&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>extravagant</trd>,
    <trd>verstiegen</trd>,
    <trd>abgehoben</trd>,
    <trd>&uuml;bergeschnappt</trd>
  </trdgrp>
  <trd lng="en">extravagant</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;raisonnable</trd>,
    <trd>excentrique<klr tip="ind"> (extravagant)</klr></trd>,
    <trd>extravagant</trd>
  </trdgrp>
  <trd lng="he">&bet;&zayin;&bet;&zayin;&nun;&yod;</trd>
  <trdgrp lng="hu">
    <trd>extravag&aacute;ns</trd>,
    <trd>k&uuml;l&ouml;nc</trd>,
    <trd>h&oacute;bortos</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#24120;&#36556;&#12434;&#36920;&#12375;&#12383;  <pr>&#12376;&#12423;&#12358;&#12365;&#12434;&#12356;&#12387;&#12375;&#12383;</pr></trd>,
    <trd>&#31361;&#39131;&#12394;  <pr>&#12392;&#12387;&#12404;&#12394;</pr></trd>,
    <trd>&#28961;&#33590;&#12394;  <pr>&#12416;&#12385;&#12419;&#12394;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ekstrawagancki</trd>,
    <trd>dziwaczny</trd>,
    <trd>cudaczny</trd>
  </trdgrp>
  <trd lng="ro">ciudat</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_k;&c_s;&c_t;&c_r;&c_a;&c_v;&c_a;&c_g;&c_a;&c_n;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_ch;&c_u;&c_d;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>extravagantn&yacute;</trd>,
    <trd>v&yacute;stredn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_je;&c_k;&c_s;&c_t;&c_r;&c_a;&c_v;&c_a;&c_gu;&c_a;&c_n;&c_t;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_z;&c_v;&c_i;&c_ch;&c_a;&c_j;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_i;&c_g;&c_a;&c_d;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_ch;&c_u;&c_d;&c_je;&c_r;&c_n;&c_a;&c_c;&c_mol;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20360; <pr>ch&#464;</pr></trd>,
    <trd>&#22823;&#25163;&#31508; <pr>d&agrave;sh&#466;ub&#464;</pr></trd>,
    <trd>&#38753; <pr>m&#464;</pr></trd>,
    <trd>&#24533; <pr>t&agrave;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ekstra1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ekstra1.0i.iu">
    <dif>
      <tld lit="E"/>e konduti:
      <ekz>
        oni devas esti tre atenta kaj ne tro <tld/>i <klr>[&#8230;sed]</klr> &scirc;i konstatis, ke
        tute certe la pasantoj ne vidas la flugantinon, neniu levis la kapon
        <fnt>
          <bib>MkM</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dand.0i">dandi</ref>,
      <ref cel="snob.0i">snobi</ref>,
      <ref cel="parad.0i.FIG">paradi<sncref/></ref>,
      <ref cel="pav.0i.FIG">pavi<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_y;&c_c;&c_mol; <ind>&c_e;&c_k;&c_s;&c_t;&c_r;&c_a;&c_v;&c_a;&c_g;&c_a;&c_n;&c_t;&c_n;&c_y;&c_m;</ind></trd>
  <trd lng="ca">extravagar</trd>
  <trdgrp lng="cs">
    <trd>chovat se v&yacute;st&rcaron;edn&ecaron;</trd>,
    <trd>tropit</trd>,
    <trd>vyv&aacute;d&ecaron;t</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sich <ind>extravagant</ind> benehmen</trd>,
    <trd><ind>verstiegen</ind> sein</trd>,
    <trd><ind>&uuml;berspannt</ind> handeln</trd>,
    <trd>&uuml;berschnappen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;raisonner</trd>,
    <trd>extravaguer</trd>
  </trdgrp>
  <trd lng="he">&lamed;&bet;&zayin;&bet;&zayin;</trd>
  <trdgrp lng="hu">
    <trd>extravag&aacute;ns</trd>,
    <trd>k&uuml;l&ouml;nck&ouml;dik</trd>,
    <trd>h&oacute;bortos</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#28961;&#33590;&#12434;&#12377;&#12427;  <pr>&#12416;&#12385;&#12419;&#12434;&#12377;&#12427;</pr></trd>,
    <trd>&#24120;&#36556;&#12434;&#36920;&#12377;&#12427;  <pr>&#12376;&#12423;&#12358;&#12365;&#12434;&#12356;&#12387;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zachowywa&cacute; si&eogonek; <ind>ekstrawagancko</ind></trd>,
    <trd>zachowywa&cacute; si&eogonek; <ind>dziwacznie</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_je;&c_s;&c_t;&c_i; &c_s;&c_je;&c_b;&c_ja; &c_e;&c_k;&c_s;&c_t;&c_r;&c_a;&c_v;&c_a;&c_g;&c_a;&c_n;&c_t;&c_n;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>podivne</trd>,
    <trd>v&yacute;stredne sa spr&aacute;va&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_v;&c_o;&c_d;&c_i;&c_t;&c_i;&c_s;&c_ja; &c_je;&c_k;&c_s;&c_t;&c_r;&c_a;&c_v;&c_a;&c_gu;&c_a;&c_n;&c_t;&c_n;&c_o;</trd>,
    <trd>&c_n;&c_je;&c_z;&c_v;&c_i;&c_ch;&c_a;&c_j;&c_n;&c_o;</trd>,
    <trd>&c_u;&c_s;&c_u;&c_p;&c_je;&c_r;&c_je;&c_ch; &c_z;&c_a;&c_g;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_p;&c_r;&c_i;&c_j;&c_n;&c_ja;&c_t;&c_i;&c_m; &c_z;&c_v;&c_i;&c_ch;&c_a;&c_ja;&c_m;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_v;&c_i;&c_l;&c_a;&c_m;</trd>,
    <trd>&c_n;&c_o;&c_r;&c_m;&c_ib;</trd>,
    <trd>&c_m;&c_o;&c_d;&c_ib;</trd>
  </trdgrp>
</drv>
