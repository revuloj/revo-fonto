      </dif>
      <trdgrp lng="de">
        <trd>Musikschule</trd>,
        <trd>Musikhochschule</trd>
      </trdgrp>
      <trd lng="fa">&a_h;&a_n;&a_r;&f_k;&a_d;&a_h;&a_hamza_sure; &a_m;&a_w;&a_s;&f_y;&a_q;&f_y;</trd>
    </subsnc>
    <subsnc mrk="konser2.0o.TEA">
      <uzo tip="fak">TEA</uzo>
      <dif>
        Supera lernejo pri teatra arto:
        <ekz>
          mi ekstudis en dram-<tld/>o en Brno
          <fnt>
            <aut>F. Ko&zcaron;&iacute;k, trad. J. Karen kaj J. Vondrou&scaron;ek</aut>,
            <vrk>La plej eminenta inter Pierotoj</vrk>,
            <lok>[2001?]</lok>
