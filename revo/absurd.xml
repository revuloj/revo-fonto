<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>absurd</rad>/a
</kap>
<drv mrk="absurd.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Kontra&ubreve;a al la komuna sa&gcirc;o:
      <ekz>
        la ideo pri la senfina movo estas <tld/>a;
      </ekz>
      <ekz>
        ekzistas grandega nombro da vortoj <klr>(fremdaj)</klr>, kiuj al
        &ccirc;iuj estas konataj sen ellernado, kaj kiujn ne uzi estus rekta
        <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">MAT</uzo>
        oni povas pruvi tiun geometrian proponon per <ind><tld/>o</ind>
        <fnt><bib>PIV1</bib></fnt>
        <klr>(per <ref cel="redukt.0oalabsurdo">redukto al
        <tld/>o</ref>)</klr>;
        <trd lng="fr">absurde<klr tip="ind"> (subst.)</klr></trd>
        <trd lng="de">Widerspruch</trd>
      </ekz>
      <ekz>
        vi diras <ind><tld/>a&jcirc;o</ind>jn;
        <trdgrp lng="de">
          <trd>Unsinn</trd>,
          <trd>Ungereimtheit</trd>,
          <trd>Widersinn</trd>
        </trdgrp>
        <trd lng="fr">absurdit&eacute;</trd>
      </ekz>
      <ekz>
        <ind><tld/>eco</ind>.
        <trdgrp lng="de">
          <trd>Absurdit&auml;t</trd>,
          <trd>Unsinnigkeit</trd>,
          <trd>Widersinnigkeit</trd>
        </trdgrp>
        <trd lng="fr">absurdit&eacute;</trd>
      </ekz>
    </dif>
    <adm>
      La ekz MAT uzas la terminon "propono" (nun "propozicio").
      <aut>MB</aut>
    </adm>
    <refgrp tip="vid">
      <ref cel="senc.sen0a">sensenca</ref>,
      <ref cel="raci">kontra&ubreve;racia</ref>,
      <ref cel="dir.kontraux0i">memkontra&ubreve;dira</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_s;&c_u;&c_r;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_d;&c_a;&c_r;&c_e;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_ja;&c_z;&c_g;&c_l;&c_u;&c_z;&c_d;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>absurd</trd>,
    <trd>ungereimt</trd>,
    <trd>unsinnig</trd>,
    <trd>widersinnig</trd>,
  </trdgrp>
  <trd lng="en">absurd</trd>
  <trd lng="es">absurdo</trd>
  <trdgrp lng="fr">
    <trd>absurde<klr tip="ind"> (adj.)</klr></trd>,
    <trd>aberrant</trd>,
    <trd>illogique</trd>,
    <trd>farfelu</trd>,
    <trd>fou<klr tip="ind"> (aberrant)</klr></trd>,
    <trd>idiot<klr tip="ind"> (aberrant)</klr></trd>,
    <trd>incoh&eacute;rent</trd>,
    <trd>stupide<klr tip="ind"> (aberrant)</klr></trd>
  </trdgrp>
  <trd lng="hu">abszurd</trd>
  <trd lng="it">assurdo</trd>
  <trd lng="nl">absurd</trd>
  <trd lng="pl">absurdalny</trd>
  <trdgrp lng="pt">
    <trd>absurdo</trd>,
    <trd>disparatado</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_b;&c_s;&c_u;&c_r;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_l;&c_je;&c_p;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">absurdn&yacute;</trd>
  <trdgrp lng="sv">
    <trd>absurd</trd>,
    <trd>orimlig</trd>
  </trdgrp>
</drv>
</art>
