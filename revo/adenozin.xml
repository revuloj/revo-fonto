<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: adenozin.xml,v 1.3 2023/10/26 09:24:14 afido Exp $">
    <kap>
      <rad>adenozin</rad>/o
      <fnt><bib>PIV2</bib></fnt>
    </kap>

<drv mrk="adenozin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="nukleozid.0o">Nukleozido</ref> konsistanta el <ref tip="prt"
      cel="adenin.0o">adenino</ref>
      kaj <ref tip="prt" cel="riboz.0o">ribozo</ref>, trovi&gcirc;anta ekz-e en
      ADP, ATP kaj <ref tip="malprt" cel="acid.ribonuklea0o">RNA</ref>:
      <ekz>
        trifosfata <tld/>o, estas la precipa fonto de energio en la korpo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007157.html">Vivo: Riboj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o same estas grava kontrolanto de la sangofluo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=590165">Adenozino</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/e/e8/Adenosin.svg" prm="PD" lrg="320">struktura formulo de <tld/>o
        <fnt>
          <aut>NEUROtiker, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Adenosin.svg">https://commons.wikimedia.org/wiki/File:Adenosin.svg</url></lok>
        </fnt>
    </bld>
    <trd lng="be">&c_a;&c_d;&c_e;&c_n;&c_a;&c_z;&c_y;&c_n;</trd>
    <trd lng="de">Adenosin</trd>
    <trd lng="en">adenosine</trd>
    <trd lng="fr">ad&eacute;nosine</trd>
    <trd lng="ru">&c_a;&c_d;&c_je;&c_n;&c_o;&c_z;&c_i;&c_n;</trd>
  </snc>
  <trd lng="ja">&#x30A2;&#x30C7;&#x30CE;&#x30B7;&#x30F3;</trd>
</drv>

  </art>

