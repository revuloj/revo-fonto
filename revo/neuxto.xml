<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: neuxto.xml,v 1.23 2024/01/29 15:21:35 revo Exp $">
<kap>
  <rad>Ne&ubreve;ton</rad>/o
</kap>
<drv mrk="neuxto.0o">
  <kap><tld/>o <fnt><bib>PIV1</bib></fnt></kap>
  <snc mrk="neuxto.0o.PERS">
    <uzo tip="fak">PERS</uzo>
    <dif>
      Anglalingve:  <trd lng="en">Newton <klr tip="amb">(Isaac)</klr></trd>, 
      1643-1727,
      angla <ref tip="lst" cel="fizik.0isto" lst="voko:historiaj_personoj">fizikisto</ref> kaj astronomo:
      <ekz>
        dua <tld/>a le&gcirc;o de la movo
        <fnt>
          <aut>C. Spinola</aut>
          <vrk><url ref="https://www.youtube.com/watch?v=cS0ASrvrn8Y">VK 2020, veturado tra la interplaneda spaco</url></vrk>
          <lok>2020-12-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_N;&c_mol;&c_ju;&c_t;&c_a;&c_n;</trd>
  <trd lng="bg">&c_I;&c_s;&c_a;&c_k; <ind>&c_N;&c_ju;&c_t;&c_o;&c_n;</ind></trd>
  <trd lng="de">Newton</trd>
  <trd lng="es">Newton</trd>
  <trd lng="fr">Newton</trd>
  <trd lng="hu">Newton</trd>
  <trd lng="id">Newton</trd>
  <trd lng="it">Newton</trd>
  <trd lng="la">Newtonus</trd>
  <trd lng="nl">Isaac <ind>Newton</ind></trd>
  <trd lng="pl">Newton</trd>
  <trd lng="pt">Newton</trd>
  <trd lng="ru">&c_N;&c_mol;&c_ju;&c_t;&c_o;&c_n;</trd>
  <trdgrp lng="zh">
    <trd>&#x725B;&#x9813; <pr>Ni&#xFA;d&#xF9;n</pr></trd>,
    <trd>&#x725B;&#x987F; <pr>Ni&#xFA;d&#xF9;n</pr></trd>,
    <trd>&#x725B;&#x5428; <pr>ni&#xFA;d&#x16B;n</pr></trd>,
    <trd>&#x725B;&#x5678; <pr>ni&#xFA;d&#x16B;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="neuxto.n0o">
  <kap><tld lit="n"/>o <fnt><bib>PIV1</bib></fnt></kap>
  <mlg kod="SI">N</mlg>
  <snc mrk="neuxto.n0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      <ref tip="lst" cel="unu.mezur0o" lst="voko:mezurunuoj">Mezurunuo</ref> de
      <ref cel="fort.0o.FIZ">forto</ref> en la Internacia Sistemo de
      unuoj:
      <ekz>
        unu <tld lit="n"/>o egalas al la forto, kiu aplikate al korpo
        kun maso
        je unu <ref cel="kilogr.0o">kilogramo</ref> donas al &gcirc;i akcelon
        je unu <ref cel="metr.0oenss">metro en kvadrata sekundo</ref>.
      </ekz>
    </dif>
    <adm>
      Ial PIV enkondukis sinonimon "nutono". <aut>MB</aut>
    </adm>
  </snc>
    <trd lng="be">&c_n;&c_mol;&c_ju;&c_t;&c_a;&c_n;</trd>
    <trd lng="bg">&c_n;&c_ju;&c_t;&c_o;&c_n;</trd>
    <trd lng="ca">newton</trd>
    <trd lng="de">Newton</trd>
    <trdgrp lng="es">
      <trd>newton<klr tip="ind"> (unidad de medida)</klr></trd>,
      <trd>neutonio</trd>
    </trdgrp>
    <trd lng="fr">newton<klr tip="ind"> (unit&eacute; de mesure)</klr></trd>
    <trd lng="hu">newton</trd>
    <trd lng="id">newton</trd>
    <trd lng="it">newton<klr> (unit&agrave; di misura)</klr></trd>
    <trd lng="nl">newton</trd>
    <trdgrp lng="pl">
      <trd>newton</trd>,
      <trd>niuton</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>newton</trd>,
      <trd>N</trd>
    </trdgrp>
    <trd lng="ru">&c_n;&c_mol;&c_ju;&c_t;&c_o;&c_n;</trd>
  <trd lng="uk">&#x41D;&#x44C;&#x44E;&#x442;&#x43E;&#x43D; (&#x444;&#x456;&#x437;&#x438;&#x43A;)</trd>
  <trdgrp lng="zh">
    <trd>&#x725B;&#x5428; <pr>ni&#xFA;d&#x16B;n</pr></trd>,
    <trd>&#x725B;&#x5678; <pr>ni&#xFA;d&#x16B;n</pr></trd>
  </trdgrp>
</drv>
</art>
