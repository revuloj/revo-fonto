<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Ne&ubreve;ton</rad>/o
</kap>
<drv mrk="neuxto.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Anglalingve: Isaac <trd lng="en">Newton</trd>, 1643-1727.
      Angla fizikisto kaj astronomo.
    </dif>
  </snc>
  <trd lng="de">Newton</trd>
  <trd lng="fr">Newton</trd>
  <trd lng="ru">&c_N;&c_mol;&c_ju;&c_t;&c_o;&c_n;</trd>
</drv>
<drv mrk="neuxto.n0o">
  <kap><tld lit="n"/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de
      <ref cel="fort.0o.FIZ">forto</ref> en la Internacia Sistemo de unuoj;
      simb. N:
      <ekz>
        unu <tld lit="n"/>o egalas al la forto, kiu aplikite al korpo kun maso
        je unu <ref cel="kilogr.0o">kilogramo</ref> donas al &gcirc;i akcelon 
        je unu <ref cel="metr.0oens2">metro en kvadrata sekundo</ref>.
      </ekz>
    </dif>
    <trd lng="de">Newton</trd>
    <trd lng="fr">newton<klr tip="ind"> (unit&eacute; de mesure)</klr></trd>
    <trd lng="nl">newton</trd>
    <trd lng="ru">&c_n;&c_mol;&c_ju;&c_t;&c_o;&c_n;</trd>
    <adm>
    Ial PIV enkondukis sinonimon "nutono". <aut>MB</aut>
    </adm>
  </snc>
</drv>
</art>

