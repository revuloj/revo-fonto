<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>3</ofc>
<rad>dekoraci</rad>/o
</kap>
<drv mrk="dekora.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Artismaj ornama&jcirc;oj de &ccirc;ambro, domo, &gcirc;ardeno, urbo:
      <ekz>
        nia standardo estas <tld/>o por niaj kongresoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Dekoration</trd>,
      <trd>Zier</trd>,
      <trd>Zierrat</trd>,
      <trd>Dekor</trd>,
      <trd>Schmuck</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;coration<klr tip="ind"> (ornement)</klr></trd>,
      <trd>parure</trd>,
      <trd>ornement</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_u;&c_k;&c_r;&c_a;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_h;&c_u;&c_d;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;&c_je; &c_o;&c_f;&c_o;&c_r;&c_m;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Pentritaj toloj prezentantaj vida&jcirc;on de la loko, kie okazas teatra sceno.
    </dif>
    <trdgrp lng="de">
      <trd>Dekor</trd>,
      <trd>Dekoration</trd>,
      <trd>Theaterdekoration</trd>,
      <trd>Kulissen</trd>
    </trdgrp>
    <trd lng="fr">d&eacute;cor<klr tip="ind"> (de th&eacute;&acirc;tre)</klr></trd>
    <trd lng="ru">&c_d;&c_je;&c_k;&c_o;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
  </snc>
  <snc>
    <fnt>B</fnt>
    <ref tip="dif" cel="orden.0o">ordeno 2.</ref>
    <trdgrp lng="fr">
      <trd>d&eacute;coration<klr tip="ind"> (insigne)</klr></trd>,
      <trd>insigne<klr tip="ind"> (d&eacute;coration)</klr></trd>
    </trdgrp>
  </snc>
</drv>
<drv mrk="dekora.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Artisme ornami:
      <ekz>
        salono <tld/>ita per freskoj.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>schm&uuml;cken</trd>,
      <trd>verzieren</trd>,
      <trd>dekorieren</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;corer</trd>,
      <trd>enjoliver</trd>,
      <trd>orner</trd>,
      <trd>parer<klr tip="ind"> (orner)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_d;&c_je;&c_k;&c_o;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
</drv>
</art>
