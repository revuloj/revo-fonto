          <vrk>La in&gcirc;enia hidalgo don Quijote de La Mancha</vrk>.
          <lok>Parto 1&#170;, &ccirc;ap. 6&#170;</lok>
        </fnt>;
      </ekz><ekz>
        la a&ubreve;toro de tiuj ver<tld/>aj linioj
        <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 5a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_w;&c_d;&c_a;&c_l;&c_ju;&c_b;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_w;&c_d;&c_z;&c_ib;&c_v;&c_y;</trd>
