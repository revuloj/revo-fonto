<drv mrk="fidel.mal0ulo">
  <kap>mal<tld/>ulo</kap>
  <snc mrk="fidel.mal0ulo.KOMUNE">
    <dif>
      Homo mal<tld/>a, malkonstanta, nefidinda:
      <ekz>
        li havis tro <klr>[&#8230;]</klr> da rilatoj kun la mal<tld/>uloj kaj malamikoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la kolumo &scirc;i &ccirc;iufoje malkovras fremdajn harojn, kaj &scirc;i
        priblekas la mal<tld/>ulon
        <fnt><aut>L. Beaucaire</aut>, <vrk>Kruko kaj Baniko el
        Bervalo</vrk>, <lok>1974</lok></fnt>.
      </ekz>
    </dif>
  <trdgrp lng="id">
    <trd>orang yg tidak berbakti</trd>,
    <trd>orang yg tidak loyal</trd>,
    <trd>orang yg tidak patuh</trd>,
    <trd>orang yg tidak percaya</trd>,
    <trd>orang yg tidak setia</trd>,
    <trd>orang yg tidak taat</trd>,
    <trd><baz>khianat</baz>pengkhianat</trd>
  </trdgrp>
  </snc>
  <snc mrk="fidel.mal0ulo.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Eksterulo rilate al
      <ref tip="ant" cel="fidel.0ulo.REL"><tld/>uloj</ref>
      de iu religio; sendiulo a&ubreve; alireligiano:
      <ekz>
        la dioj montros sian forton kontra&ubreve; la malpieco de la mal<tld/>uloj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        tra la epokoj, la religiuloj &ccirc;iam invokis dian mandaton por
        masakri mal<tld/>ulojn
        <fnt>
          <aut>M. Parenti</aut>,
          <vrk><url ref="http://www.mas-eo.org/article.php3?id_article=269">
          La mito de Tibeto</url></vrk>,
          <lok>MAS, 2008-04-29</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="herez.0ulo">herezulo</ref>,
      <ref cel="pagan.0o">pagano</ref>,
      <ref cel="profan.0ulo">profanulo</ref>
    </refgrp>
    <trd lng="fr">m&eacute;cr&eacute;ant<klr> (subst.)</klr></trd>
    <trdgrp lng="id">
      <trd>orang kafir</trd>,
      <trd>orang yg tidak beriman</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_d;&c_r;&c_a;&c_d;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_n;&c_ja;&c_v;&c_je;&c_r;&c_n;&c_y; <klr>(&c_n;&c_a;&c_z;.)</klr></trd>
  </trdgrp>
  <trd lng="cs">zr&aacute;dce</trd>
  <trdgrp lng="de">
    <trd>Untreuer</trd>,
    <trd>Treuloser</trd>
  </trdgrp>
