    <trd>v&eacute;rification</trd>
  </trdgrp> 
  <trd lng="hu">ellen&odblac;riz</trd>
  <trdgrp lng="pl">
    <trd>audyt</trd>,
    <trd>diagnostyka</trd>,
    <trd>doz&oacute;r</trd>,
    <trd>inspekcja</trd>,
    <trd>kontrola</trd>,
    <trd>rewizja</trd>,
    <trd>sprawdzenie</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>audit</trd>,
    <trd>diagnostic</trd>,
    <trd>supraveghere</trd>,
    <trd>inspec&#539;ie</trd>,
    <trd>control</trd>,
    <trd>verificare</trd>
  </trdgrp>    
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_v;&c_je;&c_r;&c_k;&c_a;</trd>,
