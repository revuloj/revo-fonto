<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kaduce</rad>/o
</kap>
<drv mrk="kaduce.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Vergo provizita per du flugiletoj, &ccirc;irka&ubreve; kiu volvi&gcirc;as
      du serpentoj, atributo de Merkuro.  
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Caduceus</trd>,
     <trd>Heroldstab des Merkur</trd>,
     <trd>Merkurstab</trd>,
     <trd>&Auml;skulapstab</trd>
  </trdgrp>
  <trd lng="en">caduceus</trd>
  <trd lng="es">caduceo</trd>
  <trdgrp lng="hu">
     <trd>kaduceus</trd>,
     <trd>merk&uacute;rbot</trd>
  </trdgrp>
  <trd lng="nl">mercuriusstaf</trd>
  <trd lng="ru">&c_k;&c_a;&c_d;&c_u;&c_c;&c_je;&c_j;</trd>
</drv>
</art>
