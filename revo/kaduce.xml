<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kaduce.xml,v 1.26 2023/10/18 14:48:09 revo Exp $">
<kap>
   <rad>kaduce</rad>/o
</kap>
<drv mrk="kaduce.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">MIT</uzo>
   <snc>
    <dif>
      Vergo provizita per du flugiletoj, &ccirc;irka&ubreve; kiu
      volvi&gcirc;as du <ref tip="prt" cel="serpen.0o">serpentoj</ref>,
      atributo de <ref cel="hermes.0o.MIT">Hermeso</ref> kaj poste
      <ref cel="merkur1.0o.mita">Merkuro</ref>:
      <ekz>
        fonto en formo de <tld/>o &scirc;prucis en orfi&scirc;plena lageto anta&ubreve; li
        <fnt>
          <bib>Monato</bib>
          <aut>Steven K. Smith</aut>
          <vrk><url ref="&Monato;/2009/009659.php">La Helpanto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/5/57/Caduceus.svg" prm="PD" lrg="180">desegno de <tld/>o
        <fnt>
          <aut>Rama, Eliot Lash</aut>
          <lok><url ref="&WCW;/Caduceus">&WCW;/Caduceus</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_d;&c_u;&c_c;&c_e;&c_j;</trd>
  <trd lng="ca">caduceu</trd>
  <trd lng="cs">caduceus</trd>
  <trdgrp lng="de">
    <trd>Caduceus</trd>,
    <trd>Heroldstab des Merkur</trd>,
    <trd>Merkurstab</trd>,
    <trd>Hermesstab</trd>
  </trdgrp>
  <trd lng="en">caduceus</trd>
  <trd lng="es">caduceo</trd>
  <trdgrp lng="fa">
     <trd>&a_ayn;&a_S;&a_A;&f_y; &f_ch;&a_A;&a_w;&a_w;&a_sh;</trd>,
     <trd>&a_ayn;&a_S;&a_A;&f_y; &a_ayn;&a_T;&a_A;&a_r;&a_d;</trd>
   </trdgrp>
  <trdgrp lng="fi">
      <trd>Merkuriuksen sauva</trd>,
      <trd>caduceus</trd>
   </trdgrp>
  <trd lng="fr">caduc&eacute;e</trd>
  <trdgrp lng="hu">
      <trd>kaduceus</trd>,
      <trd>merk&uacute;rbot</trd>
   </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30AB;&#x30C9;&#x30A5;&#x30B1;&#x30A6;&#x30B9;</trd>,
    <trd>&#x30D8;&#x30EB;&#x30E1;&#x30B9;&#x306E;&#x6756;  <pr>&#x30D8;&#x30EB;&#x30E1;&#x30B9;&#x306E;&#x3064;&#x3048;</pr></trd>,
    <trd>&#x30A2;&#x30B9;&#x30AF;&#x30EC;&#x30D4;&#x30AA;&#x30B9;&#x306E;&#x6756;  <pr>&#x30A2;&#x30B9;&#x30AF;&#x30EC;&#x30D4;&#x30AA;&#x30B9;&#x306E;&#x3064;&#x3048;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
     <trd>caduceus</trd>,
     <trd>staf van Hermes</trd>,
     <trd>mercuriusstaf</trd>,
     <trd>herautsstaf</trd>,
     <trd>esculaapteken</trd>
   </trdgrp>
  <trd lng="pl">kaduceusz</trd>
  <trd lng="pt">caduceu</trd>
  <trd lng="ru">&c_k;&c_a;&c_d;&c_u;&c_c;&c_je;&c_j;</trd>
  <trd lng="sk">caduceus</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x430;&#x434;&#x443;&#x43A;&#x435;&#x439;</trd>,
    <trd>&#x43A;&#x430;&#x434;&#x443;&#x446;&#x435;&#x439; (&#x441;&#x438;&#x43C;&#x432;&#x43E;&#x43B; &#x43C;&#x435;&#x434;&#x438;&#x446;&#x438;&#x43D;&#x438;)</trd>
  </trdgrp>
</drv>
</art>
