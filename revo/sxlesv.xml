<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
   <kap>
    <rad>&Scirc;lesvig-Holstini</rad>/o
   </kap>
   <drv mrk="sxlesvigholstini.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">GEOG</uzo>
    <snc>
       <dif>
      Unu el la federaciaj &scirc;tatoj de
      <ref tip="malprt" cel="german1.G0ujo">Germanujo</ref>, situanta inter
      Norda Maro kaj Balta Maro. Regurbo estas Kielo.
       </dif>
       <trd lng="de">Schleswig-Holstein</trd>
    </snc>
</drv>

  <drv mrk="sxlesvigholstini.0ano">
    <kap><tld lit="&scirc;"/>ano</kap>
    <snc>
      <dif>
         Civitano de &Scirc;lesvig-Holstinio.
      </dif>
    </snc>
    <trd lng="de">Schleswig-Holsteiner</trd>
  </drv>

  </art>

