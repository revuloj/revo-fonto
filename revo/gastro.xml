    <trd>labu&zcaron;n&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>gurm&aacute;n</trd>,
    <trd>labu&zcaron;n&iacute;k</trd>
  </trdgrp>
  <trd lng="uk">&c_g;&c_a;&c_s;&c_t;&c_r;&#x43E;&#x43D;&#x43E;&#x43C;&#xA0;(&#x43F;&c_r;&#x43E;
