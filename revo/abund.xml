<drv mrk="abund.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="abund.mal0i.KOMUNE">
    <dif>
      Esti mal<tld/>a, malmulta, maldensa:
      <ekz>
        sama&gcirc;uloj mal<tld/>is
        <fnt>
          <bib>FSI</bib>
          <lok>I</lok>
        </fnt>;
      </ekz>
      <ekz>
        urbo, kie plezuroj mal<tld/>as, kie luksaj kafejoj estas 
        kalkuleblaj &ccirc;e la fingroj de unu mano
        <fnt>
          <aut>Alobrogo</aut>,
          <vrk>En &Gcirc;enevo dum la konferenco</vrk>,
          <lok>Esperanto, 1932-03 (379), p. 54a</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>as rimedoj por forigi la ruinojn kaj &ccirc;ion poste ordigi
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="https://www.monato.be/2006/009066.php"
              >Politiko duondetruita</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>as fabelkolektoj en Esperanto
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="https://www.monato.be/2008/009299.php"
              >Fabeloj tradiciaj kaj moderne verkitaj</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>mangeln</trd>,
    <trd><ind>knapp</ind> sein</trd>
  </trdgrp>
  <trd lng="fr">faire d&eacute;faut</trd>
</drv>

<drv mrk="abund.mal0o">
  <kap>mal<tld/>o</kap>
  <snc mrk="abund.mal0o.KOMUNE">
    <dif>
      Malmulteco, maldenso:
      <ekz>
        mal<tld/>o de pura akvo
        <fnt>
          <bib>Kon11</bib>
          <lok>Ezopo en la Moderna Mondo</lok>
        </fnt>;
      </ekz>
      <ekz>
        plenkreskuloj, nelaste pro troa laboro <klr>[&#8230;]</klr> montras nuntempe ian
        mal<tld/>on de melatonino
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.be/2012/010764.php"
              >Dormu bone, vivu bone (per melatonino)</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        klimata &scirc;an&gcirc;i&gcirc;o kontribuos al mal<tld/>o de nutra&jcirc;oj kaj al manko de dol&ccirc;a akvo
        <fnt>
          <bib>Monato</bib>
          <aut>Lenio Marobin</aut>
          <vrk><url ref="https://www.monato.be/2014/011272.php"
              >Klimata &scirc;an&gcirc;i&gcirc;o kaj konfliktoj: kia interrilato?</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="est.for0o">foresto</ref>,
      <ref cel="mank.0o.malsuficxo">manko<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_d;&c_a;&c_h;&c_o;&c_p;</trd>,
    <trd>&c_n;&c_ja;&c_s;&c_t;&c_a;&c_ch;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>escassetat</trd>,
    <trd>insufici&egrave;ncia</trd>,
    <trd>fretura</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Mangel</trd>,
    <trd>Knappheit</trd>,
    <trd>Armut</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>escaso</trd>,
    <trd>poco</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>p&eacute;nurie</trd>,
    <trd>disette</trd>,
    <trd>insuffisance</trd>,
    <trd>maigreur<klr tip="ind"> (faible abondance)</klr></trd>,
    <trd>manque<klr tip="ind"> (p&eacute;nurie)</klr></trd>,
    <trd>minceur<klr tip="ind"> (faible abondance)</klr></trd>,
    <trd>pauvret&eacute;<klr tip="ind"> (faible abondance)</klr></trd>,
    <trd>parcimonie<klr tip="ind"> (faible abondance)</klr></trd>,
    <trd>raret&eacute;<klr tip="ind"> (p&eacute;nurie)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="abund.super0i">
  <kap>super<tld/>i,
  <var><kap>tro<tld/>i</kap></var></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="abund.super0i.KOMUNE">
    <dif>
      Esti en tia kvanto, kian oni ne povas utile uzi:
      <ekz>
        kie peko plimulti&gcirc;is, graco multe pli super<tld/>is
        <fnt><bib>NT</bib>, <lok>&Rom; 5:20</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flu.super0a">superflua</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol; &c_u; <ind>&c_l;&c_ib;&c_sh;&c_k;&c_u;</ind></trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol; &c_u; <ind>&c_z;&c_b;&c_y;&c_t;&c_k;&c_u;</ind></trd>
  </trdgrp>
  <trd lng="ca">sobreabundar</trd>
  <trd lng="de">&uuml;berflie&szlig;en</trd>
  <trd lng="en">to overabound</trd>
  <trdgrp lng="fr">
    <trd>surabonder</trd>,
    <trd>d&eacute;border</trd>,
    <trd>d&eacute;ferler</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_y;&c_t;&c_mol; &c_v; <ind>&c_i;&c_z;&c_b;&c_y;&c_t;&c_k;&c_je;</ind></trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_t;&c_i; &c_p;&c_r;&c_je;&c_b;&c_a;&c_g;&c_a;&c_t;&c_i;&c_m;/&c_p;&c_r;&c_je;&c_shch;&c_je;&c_d;&c_r;&c_i;&c_m;</trd>,
    <trd>&c_b;&c_u;&c_t;&c_i; &c_v; &c_n;&c_a;&c_d;&c_l;&c_i;&c_sh;&c_k;&c_u;</trd>
  </trdgrp>
</drv>

<drv mrk="abund.super0o">
  <kap>super<tld/>o,
  <var><kap>tro<tld/>o</kap></var>,
  <var><kap>tro<tld/>eco <fnt><bib>PV</bib></fnt></kap></var></kap>  
  <snc mrk="abund.super0o.KOMUNE">
    <dif>
      Malagrabla, embarasa <tld/>o:
      <ekz>
        ni estas vere feli&ccirc;a generacio! kion do ni ne posedas en 
        tro<tld/>eco
        <fnt>
          <aut>Leono Zamenhof, trad. L. Dreher</aut>,
          <vrk>Troabundo de Inteligentularo</vrk>,
          <lok>Pola Esperantisto, 1934, p. 151a</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ib;&c_sh;&c_a;&c_k;</trd>,
    <trd>&c_z;&c_b;&c_y;&c_t;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">sobreabund&agrave;ncia</trd>
  <trd lng="cs">p&rcaron;em&iacute;ra</trd>
  <trdgrp lng="de">
    <trd>&Uuml;berfluss</trd>,
    <trd>&Uuml;berf&uuml;lle</trd>
  </trdgrp>
  <trd lng="en">overabundance</trd>
  <trdgrp lng="fr">
    <trd>surabondance</trd>,
    <trd>exc&egrave;s<klr tip="ind"> (trop-plein)</klr></trd>,
    <trd>trop-plein<klr tip="ind"> (exc&egrave;s)</klr></trd>,
    <trd>d&eacute;bauche<klr tip="ind"> (surabondance)</klr></trd>
  </trdgrp>
  <trd lng="hu">felesleg</trd>
  <trd lng="it">sovrabbondanza</trd>
  <trd lng="nl">overvloed</trd>
  <trdgrp lng="pt">
    <trd>superabund&acirc;ncia</trd>,
    <trd>excesso</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_z;&c_b;&c_y;&c_t;&c_o;&c_k;</trd>
  <trd lng="sk">nadmiera</trd>
  <trd lng="sv">&ouml;verfl&ouml;d</trd>
</drv>
