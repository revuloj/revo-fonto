  <rad>abund</rad>/a <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="abund.0a">
  <kap><tld/>a</kap>
  <snc mrk="abund.0a.mem">
    <dif>
      Estanta en pli ol sufi&ccirc;a kvanto:
      <ekz>
        <tld/>a rikolto, stilo, argumentado, programo;
      </ekz>
      <ekz>
        granda aglo kun grandaj flugiloj kaj <tld/>aj plumoj
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 17:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni havas sur la kampo <tld/>ajn provizojn da tritiko, hordeo, oleo
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 41:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        prisemebla kampo, aran&gcirc;i<klr>[ta&#8230;]</klr> apud <tld/>a akvo
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 17:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a&jcirc;on, kiun ili kolektis, kaj sian &scirc;parita&jcirc;on
        ili transportas trans la riveron de la salikoj
        <fnt><bib>MT</bib>, <lok>&Jes; 15:7</lok></fnt>;
      </ekz>
      <ekz>
        miaj amba&ubreve; okuloj <ind><tld/>e</ind> fluigas akvon
        <fnt><bib>MT</bib>, <lok>&Plo; 1:16</lok></fnt>;
        <trdgrp lng="ca">
          <trd>en abund&agrave;ncia</trd>,
          <trd>copiosament</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>abondamment</trd>,
          <trd>amplement</trd>,
          <trd>copieusement</trd>,
          <trd>&eacute;norm&eacute;ment</trd>,
          <trd>&agrave; <ind>foison</ind></trd>,
          <trd>largement</trd>,
          <trd>&agrave; <ind>profusion</ind></trd>,
          <trd>en <ind>quantit&eacute;</ind></trd>
        </trdgrp>
        <trd lng="ms">banyak</trd>
      </ekz>
      <ekz>
        la tagman&gcirc;o, kvankam rapide farita, estis tre bona, e&ccirc; <tld/>a
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekfluis larmoj, guto post guto, <tld/>aj, pezaj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a gasa lumo &ccirc;irka&ubreve;fluis tiun figuron
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la tuta 4-a jarcento estis la epoko de &ccirc;iuflanka kaj <tld/>a florado de la
        oratora arto
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        sa&gcirc;o <tld/>a, sed ne profunda
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu ripetas <tld/>e, lernas plej funde
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mult.0a">multa</ref>,
      <ref cel="amas.0a.KOMUNE">amasa</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="ban.0i">bani</ref>,
      <ref cel="hajl.0o">hajlo</ref>,
      <ref cel="mar.0o">maro</ref>,
      <ref cel="nagx.0i">na&gcirc;i</ref>,
      <ref cel="pluv.0o">pluvo</ref>,
      <ref cel="versx.0i">ver&scirc;i</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>reichlich</trd>,
      <trd>&uuml;berm&auml;&szlig;ig</trd>,
      <trd>ausgiebig</trd>
    </trdgrp>
    <trd lng="es">abundante</trd>
    <trdgrp lng="hu">
      <trd>b&odblac;</trd>,
      <trd>b&odblac;s&eacute;ges</trd>
    </trdgrp>
    <trd lng="it">abbondante</trd>
    <trd lng="nl">overvloedig</trd>
    <trdgrp lng="pl">
      <trd>obfity</trd>,
      <trd>zasobny</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>abundante</trd>,
      <trd>copioso</trd>,
      <trd>lauto</trd>,
      <trd>basto</trd>,
      <trd>farto</trd>
    </trdgrp>
    <trd lng="ro">abundent</trd>
    <trdgrp lng="sv">
      <trd>riklig</trd>,
      <trd>ymnig</trd>
    </trdgrp>
  </snc>
  <snc mrk="abund.0a.je">
    <dif>
      Entenanta pli ol sufi&ccirc;an kvanton:
      <ekz>
        <tld/>a frukto&gcirc;ardeno
        <fnt><bib>Fab3</bib>, <lok>Sub la saliko</lok></fnt>;
      </ekz>
      <ekz>
        kapoj belege kovritaj de <tld/>aj harligoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la anta&ubreve;a jaro estis tre <tld/>a koncerne konvalojn
        <fnt><bib>Fab4</bib>, <lok>Lasta son&gcirc;o de maljuna
        kverko</lok></fnt>;
      </ekz>
      <ekz>
        la vortprovizo ne estas tiel <tld/>a kiel tiu de pli modernaj libroj
        <fnt>
          <bib>KPr</bib>
          <lok>Anta&ubreve;vorto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ricx.0a">ri&ccirc;a</ref>
    </refgrp>   
  <trdgrp lng="de">
    <trd>prall <ind>gef&uuml;llt</ind></trd>,
    <trd>f&uuml;llig</trd>,
    <trd><ind>reich</ind> an</trd>,
    <trd>umfangreich</trd>
  </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_g;&c_a;&c_t;&c_y;</trd>,
    <trd>&c_sh;&c_ch;&c_o;&c_d;&c_r;&c_y;</trd>,
    <trd>&c_g;&c_u;&c_s;&c_t;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>abundant</trd>,
    <trd>exuberant</trd>,
    <trd>prof&uacute;s</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hojn&yacute;</trd>,
    <trd>notn&yacute;</trd>,
    <trd>objemn&yacute;</trd>,
    <trd>obs&aacute;hl&yacute;</trd>,
    <trd>vydatn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>abundant</trd>,
    <trd>copious</trd>,
    <trd>bountiful</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>abondant</trd>,
    <trd>ample</trd>,
    <trd>copieux</trd>,
    <trd>f&eacute;cond</trd>,
    <trd>fourni<klr tip="ind"> (copieux)</klr></trd>,
    <trd>g&eacute;n&eacute;reux</trd>,
    <trd>large</trd>,
    <trd>luxuriant</trd>,
    <trd>multiple</trd>,
    <trd>nombreux</trd>,
    <trd>opulant</trd>,
    <trd>prolixe</trd>,
    <trd>plantureux</trd>,
    <trd>riche</trd>,
    <trd>profus</trd>
  </trdgrp>
  <trd lng="he">&shin;&vav;&pe;&ayin;</trd>
  <trdgrp lng="pl">
    <trd>obfity</trd>,
    <trd>zasobny</trd>
  </trdgrp>
  <trd lng="ro">abundent</trd>
  <trd lng="ru">&c_o;&c_b;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">hojn&yacute;</trd>
<trdgrp lng="th">
    <trd>&#3629;&#3640;&#3604;&#3617;&#3626;&#3617;&#3610;&#3641;&#3619;&#3603;&#3660;</trd>,
    <trd>&#3617;&#3637;&#3617;&#3634;&#3585;&#3617;&#3634;&#3618;</trd>,
    <trd>&#3604;&#3634;&#3625;&#3604;&#3639;&#3656;&#3609;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_ja;&c_s;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_a;&c_g;&c_a;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_shch;&c_je;&c_d;&c_r;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="abund.0i">
  <kap><tld/>i</kap>
  <snc mrk="abund.0i.KOMUNE">
    <dif>
      Esti <tld/>a:
      <ekz>
        &ccirc;u ni restu en peko, por ke graco <tld/>u
        <fnt><bib>NT</bib>, <lok>&Rom; 6:1</lok></fnt>?
      </ekz>
      <ekz>
        <tld/>u al vi anka&ubreve; &ccirc;i tiu graco
        <fnt><bib>NT</bib>, <lok>&Ko2; 8:7</lok></fnt>;
      </ekz>
      <ekz>
        la lando <tld/>is je riveroj kaj lagoj
        <fnt><bib>PIV1</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>igu &ccirc;e vi la amon de unu al la alia
        <fnt><bib>NT</bib>, <lok>&Te1; 3:12</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="krev.0i">krevi</ref>,
      <ref cel="sxvel.0i">&scirc;veli</ref>,
      <ref cel="svarm.0i">svarmi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_y;&c_c;&c_mol; &c_u; <ind>&c_d;&c_a;&c_s;&c_t;&c_a;&c_t;&c_k;&c_u;</ind></trd>
  <trdgrp lng="ca">
    <trd>abundar</trd>,
    <trd>sobreeixir</trd>,
    <trd>pul&middot;lular</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>b&yacute;t hojn&yacute;</trd>,
    <trd>h&yacute;&rcaron;it</trd>,
    <trd>opl&yacute;vat</trd>,
    <trd>opl&yacute;vat nadm&iacute;ru</trd>,
    <trd>p&rcaron;ekypovat</trd>,
    <trd>p&rcaron;ekypovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><ind>reichlich</ind> vorhanden sein</trd>,
    <trd>in <ind>F&uuml;lle</ind> zur Verf&uuml;gung stehen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>abound</trd>
  </trdgrp>
  <trd lng="es">abundar</trd>
  <trdgrp lng="fr">
    <trd>abonder<klr tip="ind"> (&ecirc;tre, se montrer abondant)</klr></trd>,
    <trd>se montrer <ind>abondant</ind></trd>,
    <trd>foisonner</trd>,
    <trd>fourmiller</trd>,
    <trd>&ecirc;tre <ind>plein</ind></trd>,
    <trd>regorger</trd>
  </trdgrp>
  <trd lng="he">&lamed;&he;&yod;&vav;&tav; &bet;&shin;&pe;&ayin;</trd>
  <trd lng="pl">obfitowa&cacute;</trd>
  <trd lng="ro">abunda</trd>
  <trd lng="ru">&c_i;&c_z;&c_o;&c_b;&c_i;&c_l;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">hojne sa vyskytova&tcaron;</trd>
<trd lng="th">&#3629;&#3640;&#3604;&#3617;&#3626;&#3617;&#3610;&#3641;&#3619;&#3603;&#3660;</trd>
</drv>

<drv mrk="abund.0o">
  <kap><tld/>o</kap>
  <snc mrk="abund.0o.KOMUNE">
    <dif>
      Pli ol sufi&ccirc;a kvanto, malmanko:
      <ekz>
        la Eternulo donos al vi <tld/>on da bono en la frukto de via
        ventro
        <fnt><bib>MT</bib>, <lok>&Rea; 28:11</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>o da afliktoj
        <fnt><bib>MT</bib>, <lok>&Ijo; 14:1</lok></fnt>,
      </ekz>
      <ekz>
        da paco kaj vero
        <fnt><bib>MT</bib>, <lok>&Jer; 33:6</lok></fnt>;
      </ekz>
      <ekz>
        pro la <tld/>o <klr>(<tld/>eco)</klr> de lakto li man&gcirc;os buteron
        <fnt><bib>MT</bib>, <lok>&Jes; 7:21</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;ie kaj &ccirc;iel mi lernis la sekreton pleni&gcirc;i kaj
        malsati, havi <tld/>on kaj havi mankon
        <fnt><bib>NT</bib>, <lok>&Fil; 4:12</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_s;&c_t;&c_a;&c_t;&c_a;&c_k;</trd>,
    <trd>&c_b;&c_a;&c_g;&c_a;&c_c;&c_mol;&c_c;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>abund&agrave;ncia</trd>,
    <trd>profusi&oacute;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hojnost</trd>,
    <trd>nadbytek</trd>
  </trdgrp>
  <trd lng="de">&Uuml;berflu&szlig;</trd>
  <trdgrp lng="en">
    <trd>bounty</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>abondance</trd>,
    <trd>ampleur<klr tip="ind"> (profusion)</klr></trd>,
    <trd>foison</trd>,
    <trd>g&eacute;n&eacute;rosit&eacute;<klr tip="ind"> (profusion)</klr></trd>,
    <trd>luxuriance</trd>,
    <trd>multitude</trd>,
    <trd>nombre</trd>,
    <trd>opulence</trd>,
    <trd>profusion</trd>,
    <trd>richesse</trd>
  </trdgrp>
  <trd lng="he">&shin;&#1473;&pe;&ayin;</trd>
  <trd lng="pl">obfito&sacute;&cacute;</trd>
  <trd lng="ro">bog&abreve;&tcedil;ie</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_i;&c_l;&c_i;&c_je;</trd>,
    <trd>&c_i;&c_z;&c_o;&c_b;&c_i;&c_l;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dostatok</trd>,
    <trd>hojnos&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="abund.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="abund.0eco.KOMUNE">
    <dif>
      Stato karakterizata de <tld/>o:
      <ekz>
        jen venos sep jaroj de <tld/>eco en la tuta Egipta lando
        <fnt><bib>MT</bib>, <lok>&Gen; 41:29</lok></fnt>;
      </ekz>
      <ekz>
        el la <tld/>eco de koro la bu&scirc;o parolas
        <klr>(elver&scirc;as la bona&jcirc;ojn de la koro)</klr>
        <fnt><bib>NT</bib>, <lok>&Mat; 12:34</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_s;&c_t;&c_a;&c_t;&c_a;&c_k;</trd>,
    <trd>&c_b;&c_a;&c_g;&c_a;&c_c;&c_mol;&c_c;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>abund&agrave;ncia</trd>,
    <trd>abundor</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hojnost</trd>,
    <trd>nadbyte&ccaron;nost</trd>,
    <trd>nadm&ecaron;rnost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>F&uuml;lle</trd>,
    <trd>&Uuml;berma&szlig;</trd>
  </trdgrp>
  <trd lng="en">abundance</trd>
  <trd lng="es">abundancia</trd>
  <trdgrp lng="fr">
    <trd>abondance</trd>,
    <trd>ampleur<klr tip="ind"> (profusion)</klr></trd>,
    <trd>foison</trd>,
    <trd>g&eacute;n&eacute;rosit&eacute;<klr tip="ind"> (profusion)</klr></trd>,
    <trd>luxuriance</trd>,
    <trd>multitude</trd>,
    <trd>nombre</trd>,
    <trd>opulence</trd>,
    <trd>profusion</trd>,
    <trd>richesse</trd>
  </trdgrp>
  <trd lng="he">&shin;&pe;&ayin;</trd>
  <trd lng="hu">b&odblac;s&eacute;g</trd>
  <trd lng="it">abbondanza</trd>
  <trd lng="nl">overvloed</trd>
  <trd lng="pl">urodzaj</trd>
  <trdgrp lng="pt">
    <trd>abund&acirc;ncia</trd>,
    <trd>fartura</trd>
  </trdgrp>
  <trd lng="ro">exuberan&tcedil;&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_i;&c_l;&c_i;&c_je;</trd>,
    <trd>&c_i;&c_z;&c_o;&c_b;&c_i;&c_l;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">hojnos&tcaron;</trd>
  <trdgrp lng="sv">
    <trd>&ouml;verfl&ouml;d</trd>,
    <trd>rikedom</trd>
  </trdgrp>
<trd lng="th">&#3588;&#3623;&#3634;&#3617;&#3629;&#3640;&#3604;&#3617;&#3626;&#3617;&#3610;&#3641;&#3619;&#3603;&#3660;</trd>
</drv>

<drv mrk="abund.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="abund.mal0a.KOMUNE">
    <dif>
      En tre malgranda kvanto:
      <ekz>
        mal<tld/>aj resta&jcirc;oj de la iama tegmento estis kovritaj de musko
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>aj financaj rimedoj
        <fnt>
          <bib>EeP</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        neniu alia lingvo estas ellernebla <klr>[&#8230;]</klr> el tiel mal<tld/>a materialo
        <fnt>
          <bib>EeP</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;tonmurego <klr>[&#8230;]</klr><klr>[&#8230;]</klr> kovrita de mal<tld/>a herbaro
        <fnt>
          <bib>ChV</bib>
          <lok>5</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj poeziaj verkoj, mal<tld/>aj, estas framoj delikataj kaj elegantaj
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        en sia mal<tld/>a libertempo, li estas direktoro de la urba &hcirc;oro
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="https://www.monato.be/2009/009680.php"
              >Proksimaj kaj malproksimaj najbaroj</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="apenau.0a">apena&ubreve;a</ref>,
      <ref cel="mult.mal0a">malmulta</ref>,
      <ref cel="rar.0a.malofta">rara<sncref/></ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="dis.0a.KOMUNE">disa<sncref/></ref>,
      <ref cel="sporad.0a">sporada</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_d;&c_a;&c_s;&c_t;&c_a;&c_t;&c_k;&c_o;&c_v;&c_y;</trd>
  <trd lng="bo">&t_tha;&t_tsheg;&t_ra;&t_drengbu;&t_tsheg;&t_tha;&t_naro;&t_ra;&t_tsheg;&t_ra;&t_drengbu;&t_tsheg;</trd>
  <trdgrp lng="ca">
    <trd>escassejar</trd>,
    <trd>freturar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nedostate&ccaron;n&yacute;</trd>,
    <trd>nehojn&yacute;</trd>,
    <trd>omezen&yacute;</trd>,
    <trd>spor&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sp&auml;rlich</trd>,
    <trd>d&uuml;rftig</trd>,
    <trd>rar</trd>,
    <trd>knapp</trd>
  </trdgrp>
