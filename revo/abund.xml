<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>3</ofc>
<rad>abund</rad>/a
</kap>
<drv mrk="abund.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Estanta en pli ol sufi&ccirc;a kvanto: 
      <ekz>
        <tld/>a rikolto, stilo, argumentado, programo; 
      </ekz>
      <ekz>
        <tld/>aj larmoj<fnt>Z</fnt>,
      </ekz>
      <ekz>
        haroj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        sa&gcirc;o <tld/>a sed ne profunda
        <fnt>
          (<aut>Zamenhof</aut>:
          <vrk>Proverbaro</vrk>)
        </fnt>;
      </ekz>
      <ekz>
        miaj amba&ubreve; okuloj <tld/>e fluigas akvon
        <fnt>
          (<aut>Zamenhof</aut>:
          <vrk>Biblio</vrk>,
          <lok>Plorkanto 1, 16</lok>)
        </fnt>;
      </ekz>
      <ekz>
        kiu ripetas <tld/>e, lernas plej funde
        <fnt>
          (<aut>Zamenhof</aut>:
          <vrk>Proverbaro</vrk>)
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ricx.0a">ri&ccirc;a</ref>,
      <ref cel="mult.0a">multa</ref>,
      <ref cel="amas">amasa</ref>,
      <ref cel="svarm.0i">svarmi</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="nagx.0i">na&gcirc;i</ref>,
      <ref cel="ban.0i">bani</ref>,
      <ref cel="versx.0i">ver&scirc;i</ref>,
      <ref cel="pluv.0o">pluvo</ref>,
      <ref cel="hajl.0o">hajlo</ref>,
      <ref cel="mar.0o">maro</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>reichlich</trd>,
    <trd>&uuml;berm&auml;&szlig;ig</trd>,
    <trd>ausgiebig</trd>
  </trdgrp>
  <trd lng="en">abundant</trd>
  <trd lng="fr">abondant</trd>
  <trd lng="it">abbondante</trd>
  <trd lng="nl">overvloedig</trd>
  <trdgrp lng="pt">
      <trd>abundante</trd>,
      <trd>copioso</trd>,
      <trd>lauto</trd>,
      <trd>basto</trd>,
      <trd>farto</trd>
  </trdgrp>
  <trd lng="ru">&#1086;&#1073;&#1080;&#1083;&#1100;&#1085;&#1099;&#1081;</trd>
  <trdgrp lng="sv">
    <trd>riklig</trd>,
    <trd>ymnig</trd>
  </trdgrp>
</drv>
<drv mrk="abund.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Malmanko, sufi&ccirc;egeco:
      <ekz>
        <tld/>eco da fruktoj, da pluvo, da
        vizitantoj, da paroloj;
      </ekz>
      <ekz>
        paroli el la <tld/>(ec)o de koro
        <klr>(elver&scirc;ante sian koron)</klr>.
      </ekz>  
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>F&uuml;lle</trd>,
    <trd>&Uuml;berma&szlig;</trd>
  </trdgrp>
  <trd lng="fr">abondance</trd>
  <trd lng="it">abbondanza</trd>
  <trd lng="nl">overvloed</trd>
  <trdgrp lng="pt">
     <trd>abund&acirc;ncia</trd>,
     <trd>fartura</trd>
  </trdgrp>
  <trd lng="ru">&#1086;&#1073;&#1080;&#1083;&#1080;&#1077;</trd>
  <trdgrp lng="sv">
    <trd>&ouml;verfl&ouml;d</trd>,
    <trd>rikedom</trd>
  </trdgrp>
</drv>
<drv mrk="abund.tro0ecosuper0eco">
  <kap>tro<tld/>eco, super<tld/>eco</kap>
  <snc>
    <dif>
      Tia kvanto, kian oni ne povas utili. 
    </dif>
    <refgrp tip="sin">
      <ref cel="flu.super0a">superflua</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>&Uuml;berfluss</trd>,
    <trd>&Uuml;berf&uuml;lle</trd>
  </trdgrp>
  <trd lng="fr">surabondance</trd>
  <trd lng="it">sovrabbondanza</trd>
  <trd lng="nl">overvloed</trd>
  <trdgrp lng="pt">
     <trd>superabund&acirc;ncia</trd>,
     <trd>excesso</trd>
  </trdgrp>
  <trd lng="ru">&#1080;&#1079;&#1073;&#1099;&#1090;&#1086;&#1082;</trd>
  <trd lng="sv">&ouml;verfl&ouml;d</trd>
</drv>
<drv mrk="abund.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      En pli malgranda kvanto ol la nepre necesa: 
      <ekz>
        mal<tld/>o de nutra&jcirc;oj devigis la sie&gcirc;itojn kapitulaci.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>d&uuml;rftig</trd>,
    <trd>knapp</trd>,
    <trd>rar</trd>
  </trdgrp>
  <trd lng="fr">rare</trd>
  <trdgrp lng="it">
     <trd>scarso</trd>, 
     <trd>raro</trd>
  </trdgrp>
  <trd lng="nl">weinig</trd>
  <trdgrp lng="pt">
     <trd>escasso</trd>,
     <trd>raro</trd>
  </trdgrp>
  <trd lng="ru">&#1085;&#1077;&#1076;&#1086;&#1089;&#1090;&#1072;&#1090;&#1086;&#1095;&#1085;&#1099;&#1081;</trd>
  <trdgrp lng="sv">
    <trd>knapp</trd>,
    <trd>torftig</trd>
  </trdgrp>
</drv>
</art>
