<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: volut.xml,v 1.9 2024/01/15 06:31:41 revo Exp $">
<kap>
    <rad>volut</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="volut.0o">
  <kap><tld/>o</kap>
  <snc mrk="volut.0o.ornamo">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">TIP</uzo>
    <dif>
      Spiralforma <ref tip="super" cel="ornam.0ajxo">ornama&jcirc;o</ref>, ekz. &ccirc;e konstrua&jcirc;oj, muzikinstrumentoj,
      litertipoj:
      <ekz>
        la bildoj sur la muroj montris <tld/>ajn
        ornama&jcirc;ojn a&ubreve; skribsignojn de fremdspeca alfabeto
        <fnt>
          <aut>Micheal Ende, trad. Wolfram Diestel</aut>,
          <vrk>La Sen&ccirc;esa Rakonto</vrk>,
          <lok>&ccirc;apitro 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        dol&ccirc;a muziko <klr>[&#8230;]</klr> tenis lin najlita al la <tld/>oj de fera barilo
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470; 11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_l;&c_ju;&c_t;&c_a; <klr>(&c_a;&c_r;&c_h;.)</klr></trd>
  <trdgrp lng="cs">
    <trd>voluta</trd>,
    <trd>z&aacute;vit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schn&ouml;rkel</trd>,
    <trd>Volute</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>volute</trd>,
    <trd>scroll</trd>
  </trdgrp>
  <trd lng="fr">volute</trd>
  <trd lng="ja">&#28198;&#24059;&#35013;&#39166;  <pr>&#12358;&#12378;&#12363;&#12435;&#12381;&#12358;&#12375;&#12423;&#12367;</pr></trd>
  <trdgrp lng="sk">
    <trd>voluta</trd>,
    <trd>z&aacute;vit</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_o;&c_l;&c_ju;&c_t;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_i;&c_t;&c_o;&c_k;</trd>
  </trdgrp>
</drv>
</art>

