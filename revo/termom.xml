<art mrk="$Id: termom.xml,v 1.26 2025/04/06 18:09:50 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>termometr</rad>/o
<fnt><bib>JED</bib></fnt></kap>
<drv mrk="termom.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="instru2.0o">Instrumento</ref>
      por mezuri la gradon de varmo:
      <ekz>
        kiam la <tld/>o indikas pli ol 30 celsiajn gradojn, mi &scirc;vitas e&ccirc; se mi sidas tute senmove
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2003/008190.html">Varmego suferiga</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        unuafoje dum epidemio oni mezuris la temperaturon helpe de senkontaktaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2005/008721.php">De kie venis kaj 
          kial &ccirc;esis netipa pne&ubreve;monio?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        konata ilo, kies funkciado ja bazi&gcirc;as sur la dilati&gcirc;o kaj kuntiri&gcirc;o de 
        likva&jcirc;oj pro variado de temperaturo, estas <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011332.php">Dilati&gcirc;o,
          kuntiri&gcirc;o ... kaj eksperimento por infana amuzi&gcirc;o</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Anka&#365; eblas diri <ke>varmomezurilo</ke>, <ke>temperaturmezurilo</ke>.
  </rim>

  <trd lng="be">&c_t;&c_e;&c_r;&c_m;&c_o;&c_m;&c_e;&c_t;&c_a;&c_r;</trd>
  <trd lng="bg">&c_t;&c_je;&c_r;&c_m;&c_o;&c_m;&c_je;&c_t;&c_malmol;&c_r;</trd>
  <trd lng="ca">term&ograve;metre</trd>
  <trdgrp lng="cs">
    <trd>teplom&ecaron;r</trd>,
    <trd>termometr</trd>
  </trdgrp>
  <trd lng="de">Thermometer</trd>
  <trd lng="en">thermometer</trd>
  <trd lng="es">term&oacute;metro</trd>
  <trd lng="fr">thermom&egrave;tre</trd>
  <trd lng="hu">h&odblac;m&eacute;r&odblac;</trd>
  <trdgrp lng="ja">
    <trd>&#28201;&#24230;&#35336;  <pr>&#12362;&#12435;&#12393;&#12369;&#12356;</pr></trd>,
    <trd>&#23506;&#26262;&#35336;  <pr>&#12363;&#12435;&#12384;&#12435;&#12369;&#12356;</pr></trd>,
    <trd>&#20307;&#28201;&#35336;  <pr>&#12383;&#12356;&#12362;&#12435;&#12369;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="ko">&#50728;&#46020;&#44228;</trd>
  <trd lng="nl">thermometer</trd>
  <trd lng="pl">termometr</trd>
  <trdgrp lng="pt">
    <trd>term&ocirc;metro</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_je;&c_r;&c_m;&c_o;&c_m;&c_je;&c_t;&c_r;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_d;&c_u;&c_s;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">teplomer</trd>
  <trd lng="sv">termometer</trd>
  <trdgrp lng="tr">
    <trd>derece</trd>,
    <trd>termometre</trd>
  </trdgrp>
  <trd lng="uk">&#1090;&#1077;&#1088;&#1084;&#1086;&#1084;&#1077;&#1090;&#1088;</trd>
  <trdgrp lng="zh">
    <trd>&#28201;&#24230;&#35745; <pr>w&#275;nd&#249;j&#236;</pr></trd>,
    <trd>&#28331;&#24230;&#35336; <pr>w&#275;nd&#249;j&#236;</pr></trd>,
    <trd>&#28201;&#24230;&#34920; <pr>w&#275;nd&#249;bi&#462;o</pr></trd>,
    <trd>&#28331;&#24230;&#34920; <pr>w&#275;nd&#249;bi&#462;o</pr></trd>
  </trdgrp>
</drv>
