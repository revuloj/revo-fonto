<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>oaz</rad>/o
</kap>
<drv mrk="oaz.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Izola loko, verda kaj kvaza&ubreve;insula meze en la sabla maro de la
      dezerto, kaj kovrita de vegeta&jcirc;oj pro ekzisto de fontoj:
      <ekz>
        la <tld/>oj de Saharo.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      &Scirc;irmejo, rifu&gcirc;ejo, ripozejo:
      <ekz>
        en mezo de la lando, superkovrita de la fluo de la Barbaroj,
        restis kelkaj <tld/>oj de civilizo;
      </ekz>
      <ekz>
        tio &scirc;ajnis <tld/>o de silento meze de brua urbo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_a;&c_z;&c_ib;&c_s;</trd>
  <trd lng="ca">oasi</trd>
  <trd lng="de">Oase</trd>
  <trd lng="el">&omikron_ton;&alfa;&sigma;&eta;</trd>
  <trd lng="en">oasis</trd>
  <trd lng="es">oasis</trd>
  <trd lng="fr">oasis</trd>
  <trd lng="hu">o&aacute;zis</trd>
  <trd lng="it">oasi</trd>
  <trd lng="nl">oase</trd>
  <trd lng="pt">o&aacute;sis</trd>
  <trd lng="ru">&c_o;&c_a;&c_z;&c_i;&c_s;</trd>
  <trd lng="sv">oas</trd>
</drv>
</art>
