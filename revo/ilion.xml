<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ilion.xml,v 1.3 2024/09/25 09:42:00 revo Exp $">
<kap>
    -<rad>ilion</rad>/ <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="ilion.0">
  <kap>-<tld/></kap>
  <snc mrk="ilion.0.sufikso">
    <dif>
      Sufikso uzebla kun nombroj signifante 10 je la potenco sesoble <frm am="x">x</frm>:
      <ekz>
        <frm am="10^18">10<sup>18</sup></frm>
           &dash;	tri<tld/>o,
        <frm am="10^36">10<sup>36</sup></frm>
          &dash; ses<tld/>o
        <fnt><bib>Viki</bib>, <lok>Vortoj por grandaj
        nombroj</lok></fnt>
      </ekz>
    </dif>
  </snc>
<trdgrp lng="be">
  <trd>-&c_ib;&c_l;&c_mol;&c_jo;&c_n;</trd>,
  <trd>-&c_y;&c_l;&c_mol;&c_jo;&c_n;</trd>
</trdgrp>
</drv>
</art>

