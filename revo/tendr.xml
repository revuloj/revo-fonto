<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tendr.xml,v 1.21 2023/10/21 13:02:26 revo Exp $">
<kap>
  <rad>tendr</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tendr.0o">
  <kap><tld/>o</kap>
  <snc mrk="tendr.0o.FER">
    <uzo tip="fak">FER</uzo>
    <dif>
      Speciala <ref tip="super" cel="vagon.0o">vagono</ref>, ligita senpere al la 
      lokomotivo kaj sur kiu trovi&gcirc;as rezervoj de <ref cel="karb.0o">karbo</ref> kaj akvo,
      necesaj por la lokomotivo:
      <ekz>
        la ma&scirc;inisto grimpis teren kaj malkuplis
        la lokomotivon kaj la <tld/>on
        <fnt>
          <aut>O. Henry, trad. R. F&ouml;ssmeier</aut>,
          <vrk><url ref="&Sezonoj;2015/02/o-henry/">
          La vojoj, kiujn ni iras</url></vrk>,
          <lok>La Ondo de Esperanto, 2015:2 (244)</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_n;&c_d;&c_e;&c_r; <klr tip="amb">(&c_v;&c_a;&c_g;&c_o;&c_n;)</klr></trd>
  <trd lng="ca">t&egrave;nder</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;&iacute;v&ecaron;s</trd>,
    <trd>tendr</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Tender</trd>,
    <trd>Kohlewagen</trd>
  </trdgrp>
  <trd lng="en">tender</trd>
  <trd lng="es">t&eacute;nder</trd>
  <trd lng="fr">tender</trd>
  <trd lng="hu">szerkocsi</trd>
  <trd lng="ja">&#x70AD;&#x6C34;&#x8ECA;  <pr>&#x3059;&#x307F;&#x3059;&#x3044;&#x3057;&#x3083;</pr></trd>
  <trd lng="nl">tender</trd>
  <trd lng="pl">tender</trd>
  <trdgrp lng="pt">
    <trd>t&ecirc;nder</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_n;&c_d;&c_je;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>pr&iacute;ves</trd>,
    <trd>tender</trd>
  </trdgrp>
  <trd lng="uk">&c_t;&c_je;&c_n;&c_d;&c_je;&c_r;</trd>
</drv>
</art>
