<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tendr</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="tendr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FER</uzo>
  <snc>
    <dif>
      Speciala vagono, ligita senpere al la lokomotivo kaj sur kiu
      trovi&gcirc;as rezervoj de karbo kaj akvo, necesaj por la lokomotivo.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_n;&c_d;&c_e;&c_r; <klr 
tip="amb">(&c_v;&c_a;&c_g;&c_o;&c_n;)</klr></trd>
  <trdgrp lng="de">
    <trd>Tender</trd>,
    <trd>Kohlewagen</trd>
  </trdgrp>
  <trd lng="en">tender</trd>
  <trd lng="es">t&eacute;nder</trd>
  <trd lng="hu">szerkocsi</trd>
  <trd lng="nl">tender</trd>
  <trd lng="pl">tender</trd>
  <trdgrp lng="pt">
    <trd>t&ecirc;nder</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_n;&c_d;&c_je;&c_r;</trd>
</drv>
</art>
