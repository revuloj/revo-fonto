<vortaro>
<art mrk="$Id: ingot.xml,v 1.11 2022/03/21 15:27:49 revo Exp $">
<kap>
  <rad>ingot</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ingot.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Briko da metalo uzata por konservo a&ubreve; kiel valuto:
      <ekz>
        ili vidis, ke <klr>...</klr> brilas ar&gcirc;entaj <tld/>oj, kaj
        anka&ubreve;
        pesileto
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 51a, p. 159a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">lingot</trd>
  <trd lng="cs">ingot</trd>
  <trd lng="de">Barren<klr> (Metall)</klr></trd>
  <trd lng="en">ingot</trd>
  <trd lng="es">lingote</trd>
  <trd lng="fa">&a_sh;&a_m;&a_sh;</trd>
  <trd lng="fr">lingot</trd>
  <trdgrp lng="hu">
    <trd>f&eacute;mtusk&oacute;</trd>,
    <trd>&ouml;ntecs</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>baar<klr tip="ind"> (ingot)</klr></trd>,
    <trd>ingot</trd>
  </trdgrp>
  <trd lng="pt">lingote</trd>
  <trd lng="sk">ingot</trd>
  <trdgrp lng="uk">
    <trd>&#x437;&#x43B;&#x438;&#x432;&#x43E;&#x43A;</trd>,
    <trd>&#x437;&#x43B;&#x438;&#x442;&#x43E;&#x43A; (&#x431;&#x43B;&#x430;&#x433;&#x43E;&#x440;&#x43E;&#x434;&#x43D;&#x43E;&#x433;&#x43E; &#x43C;&#x435;&#x442;&#x430;&#x43B;&#x443;)</trd>
  </trdgrp>
</drv>
</art>

