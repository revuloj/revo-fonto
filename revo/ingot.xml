<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ingot.xml,v 1.14 2023/11/19 10:22:13 revo Exp $">
<kap>
  <rad>ingot</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ingot.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Metal<ref cel="brik.0o.formo">briko</ref>, uzata por konservo a&ubreve; kiel valuto:
      <ekz>
        ili vidis, ke <klr>[&#x2026;]</klr> brilas ar&gcirc;entaj <tld/>oj, kaj
        anka&ubreve; pesileto
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 51a, p. 159a</lok></fnt>;
      </ekz>
      <ekz>
        ilaro, rezervita por niaj specialaj valora&jcirc;-ekspedoj, kiel ekzemple por or- a&ubreve;
        platen-<tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Filippo</aut>
          <vrk><url ref="&Monato;/2006/008898.php">Moderna po&scirc;to</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_l;&c_ib;&c_t;&c_a;&c_k;</trd>
  <trd lng="ca">lingot</trd>
  <trd lng="cs">ingot</trd>
  <trd lng="de">Barren<klr> (Metall)</klr></trd>
  <trd lng="en">ingot</trd>
  <trd lng="es">lingote</trd>
  <trd lng="fa">&a_sh;&a_m;&a_sh;</trd>
  <trd lng="fr">lingot</trd>
  <trdgrp lng="hu">
    <trd>f&eacute;mtusk&oacute;</trd>,
    <trd>&ouml;ntecs</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>baar<klr tip="ind"> (ingot)</klr></trd>,
    <trd>ingot</trd>
  </trdgrp>
  <trd lng="pt">lingote</trd>
  <trd lng="sk">ingot</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_l;&c_i;&c_v;&c_o;&c_k;</trd>,
    <trd>&c_z;&c_l;&c_i;&c_t;&c_o;&c_k; (&c_b;&c_l;&c_a;&c_g;&c_o;&c_r;&c_o;&c_d;&c_n;&c_o;&c_g;&c_o;
    &c_m;&c_je;&c_t;&c_a;&c_l;&c_u;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x92CC; <pr>d&#xEC;ng</pr></trd>,
    <trd>&#x9320; <pr>d&#xEC;ng</pr></trd>,
    <trd>&#x9301; <pr>k&#xE8;</pr></trd>
  </trdgrp>
</drv>
</art>

