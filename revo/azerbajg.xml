<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>azerbaj&gcirc;an</rad>/o
</kap>

<drv mrk="azerbajg.0o">
  <kap>
    <tld lit="A"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="azerbajg.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <mlg>AZ</mlg>
    <dif>
      &Scirc;tato en la centra Azio kun &ccirc;efurbo Bakuo.
      Oficiala nomo: <em>Respubliko de Azerbaj&gcirc;ano</em>.
      &Gcirc;i limas kun <ref tip="vid"
      cel="armen.0ujo.GEOG">Armenujo</ref>,
      <ref tip="vid" cel="iran.0o.GEOG">Irano</ref>,
      <ref tip="vid" cel="kartvel.K0ujo">Kartvelujo</ref>,
      <ref tip="vid" cel="rus.R0ujo">Rusujo</ref>,
      <ref tip="vid" cel="uzbek.0ujo">Turkujo</ref>
      kaj <ref tip="vid" cel="kaspi.0o">Kaspio</ref>.
    </dif>
    <trdgrp lng="pl">
      <trd>Azerbejd&zdot;an</trd>,
      <trd>Republika <ind>Azerbejd&zdot;anu</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

<!--
$Log$
-->
</vortaro>