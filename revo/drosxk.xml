<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <rad>dro&scirc;k</rad>/o
    <fnt>Z</fnt>
    <fnt><bib>ZR</bib></fnt>
  </kap>
  <drv mrk="drosxk.0o">
    <kap><tld/>o</kap>
    <snc>
      <dif>
        Speco de rusa <ref tip="super" cel="vetur.0ilo">veturilo</ref>,
        <ref tip="vid" cel="fiakr.0o">fiakro</ref>, 
        lu<ref tip="vid" cel="kalesx.0o">kale&scirc;o</ref>:
        <ekz>
          preni (lui) <tld/>on<fnt>Z</fnt>.
        </ekz>  
      </dif>
    </snc>
    <trd lng="be">&c_d;&c_r;&c_o;&c_zh;&c_k;&c_ib;</trd>
    <trd lng="de">Droschke</trd>
    <trdgrp lng="en">
      <trd>drosky</trd>,
      <trd>droshky</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>droske</trd>,
      <trd>b&eacute;rkocsi</trd>
    </trdgrp>
    <trd lng="nl">drosjke</trd>
    <trd lng="pl">doro&zdot;ka</trd>
    <trdgrp lng="pt">
      <trd>fiacre</trd>,
      <trd>carruagem de aluguel</trd>,
      <trd>"<ind>droshky</ind>"</trd>,
    </trdgrp>
    <trd lng="ru">&c_d;&c_r;&c_o;&c_zh;&c_k;&c_i;</trd>
  </drv>
</art>
