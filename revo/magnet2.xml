<art mrk="$Id: magnet2.xml,v 1.19 2024/01/25 20:10:33 revo Exp $">
<kap>
  <rad>magnetit</rad>/o
</kap>
<drv mrk="magnet2.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">MIN</uzo>
    <dif>
      Natura <ref tip="super" cel="oksid.0o">oksido</ref>
      de fero, <frm>Fe<sub>3</sub>O<sub>4</sub></frm>, posedanta magnetajn ecojn:
      <ekz>
        vanado nature trovi&gcirc;as <klr>[&#x2026;]</klr> kiel malpuriga&jcirc;o en
        <tld/>o, feroerco, kiu
        povas konsisti el &gcirc;is 1&#x202F;&ndash;&#x202F;2&#x202F;% da vanado
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=22284">Vanado</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="hemati.0o">hematito</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_g;&c_n;&c_e;&c_t;&c_y;&c_t;</trd>,
    <trd>&c_m;&c_a;&c_g;&c_n;&c_ib;&c_t;&c_n;&c_y; <ind>&c_zh;&c_a;&c_l;&c_ja;&c_z;&c_mol;&c_n;&c_ja;&c_k;</ind></trd>
  </trdgrp>
  <trd lng="ca">magnetita</trd>
  <trd lng="de">Magnetit</trd>
  <trd lng="en">magnetite</trd>
  <trd lng="es">magnetita</trd>
  <trd lng="fr">magn&eacute;tite</trd>
  <trd lng="hu">magnetit</trd>
  <trd lng="ja">&#x78C1;&#x9244;&#x9271;  <pr>&#x3058;&#x3066;&#x3063;&#x3053;&#x3046;</pr></trd>
  <trdgrp lng="nl">
    <trd>magnetiet</trd>,
    <trd>magneetijzersteen</trd>
  </trdgrp>
  <trd lng="pl">magnetyt</trd>
  <trdgrp lng="pt">
    <trd>magnetita</trd>,
    <trd>coulsonita</trd>,
    <trd>ferro-ferrita</trd>,    
    <trd>pedra-&iacute;m&atilde;</trd>,    
    <trd>silfberguita</trd>
  </trdgrp>
  <trd lng="ro">magnetit</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_g;&c_n;&c_je;&c_t;&c_i;&c_t;</trd>,
    <trd>&c_m;&c_a;&c_g;&c_n;&c_i;&c_t;&c_n;&c_y;&c_j; &c_zh;&c_je;&c_l;&c_je;&c_z;&c_n;&c_ja;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_a;&c_g;&#x43D;&c_je;&c_t;&c_i;&c_t;</trd>,
    <trd>&#x43C;&c_a;&c_g;&#x43D;&c_ib;&c_t;&#x43D;&c_i;&c_j; &c_z;&c_a;&#x43B;&c_ib;&c_z;&#x43D;&#x44F;&#x43A;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x78C1;&#x9435;&#x7926; <pr>c&#xED;ti&#x11B;ku&#xE0;ng</pr></trd>,
    <trd>&#x78C1;&#x94C1;&#x77FF; <pr>c&#xED;ti&#x11B;ku&#xE0;ng</pr></trd>
  </trdgrp>
</drv>
</art>
