<kap>
  <rad>endokardit</rad>/o
</kap>
<drv mrk="endoka2.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="inflam.0o" lst="voko:malsanoj">Inflamo</ref>
      de <ref cel="endoka.0o">endokardo</ref>:
      <ekz>
        embolioj povas formi&gcirc;i plej ofte en la koro pro kormalsanoj kiel 
        <klr>[&#x2026;]</klr> <tld/>o kaj malsano de la kora muskolo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=51997">Apopleksio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Robert Delaunay</nom> <klr>[&#x2026;]</klr> la 20-an de oktobro 1908 <klr>[&#x2026;]</klr> estis
        liberigita el militservo pro &leftquot;funkciaj perturboj en la koro&rightquot;, kaj &leftquot;<tld/>o&rightquot;,
        poste li revenis al Parizo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=407986">Robert Delaunay</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_n;&c_d;&c_a;&c_k;&c_a;&c_r;&c_d;&c_y;&c_t;</trd>
  <trd lng="bg">&c_je;&c_n;&c_d;&c_o;&c_k;&c_a;&c_r;&c_d;&c_i;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>endokarditida</trd>,
    <trd>endokarditis</trd>,
    <trd>z&aacute;n&ecaron;t vnit&rcaron;n&iacute; bl&aacute;ny srde&ccaron;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Endokarditis<klr> (Med.)</klr></trd>,
    <trd>Herzinnenhautentz&uuml;ndung</trd>
  </trdgrp>
  <trd lng="en">endocarditis</trd>
  <trd lng="fr">endocardite</trd>
  <trd lng="hu">sz&iacute;vbelh&aacute;rtya-gyullad&aacute;s</trd>
  <trd lng="id">endokarditis</trd>
  <trd lng="ja">&#x5FC3;&#x5185;&#x819C;&#x708E;  <pr>&#x3057;&#x3093;&#x306A;&#x3044;&#x307E;&#x304F;&#x3048;&#x3093;</pr></trd>
  <trd lng="nl">endocarditis</trd>
  <trd lng="pl">wsierdzia zapalenie</trd>
  <trd lng="pt">endocardite</trd>
  <trd lng="ru">&c_e;&c_n;&c_d;&c_o;&c_k;&c_a;&c_r;&c_d;&c_i;&c_t;</trd>
  <trd lng="sk">z&aacute;pal vn&uacute;tornej srdcovej blany</trd>
  <trd lng="zh">&#x5FC3;&#x5167;&#x819C;&#x708E; <pr>x&#x12B;nn&#xE8;im&#xF3;y&#xE1;n</pr></trd>
</drv>
