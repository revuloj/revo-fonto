<kap>
  <rad>mandragor</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="mandra.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="solana.0oj">solanacoj</ref>
      (<trd lng="la">Mandragora</trd>); plurjara
      <ref tip="lst" cel="herb.0o" lst="voko:solanoparencoj">herbo</ref>
      kun grandaj kaj lar&gcirc;aj ovoformaj rozete aran&gcirc;itaj folioj de
      malagrabla odoro kaj gusto kaj kun dika karna, ofte forki&gcirc;anta
      radiko, aspektante ne malofte kiel homa figuro:
      <ekz>
        Ruben iris en la tempo de rikoltado de tritiko kaj trovis
        <tld/>ojn sur la kampo
        <fnt><bib>MT</bib>, <lok>&Gen; 30:14</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_d;&c_r;&c_a;&c_g;&c_o;&c_r;&c_a;</trd>
  <trd lng="ca">mandr&agrave;gora</trd>
  <trdgrp lng="cs">
    <trd>div&yacute; mu&zcaron;&iacute;k</trd>,
    <trd>mandragora</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Allraune</trd>,
    <trd>Mandragora</trd>
  </trdgrp>
  <trd lng="en">mandrake</trd>
  <trd lng="fr">mandragore</trd>
  <trd lng="hu">mandrag&oacute;ra</trd>
  <trdgrp lng="ja">
    <trd>&#x30DE;&#x30F3;&#x30C9;&#x30E9;&#x30B4;&#x30E9;</trd>,
    <trd>&#x30DE;&#x30F3;&#x30C9;&#x30E9;&#x30B4;&#x30E9;&#x306E;&#x6839;  <pr>&#x30DE;&#x30F3;&#x30C9;&#x30E9;&#x30B4;&#x30E9;&#x306E;&#x306D;</pr></trd>
  </trdgrp>
  <trd lng="nl">mandragore</trd>
  <trd lng="pl">mandragora</trd>
  <trd lng="pt">mandr&aacute;gora</trd>
  <trdgrp lng="ro">
    <trd>m&abreve;tr&abreve;gun&abreve;</trd>,
    <trd>iarba codrilor</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_a;&c_n;&c_d;&c_r;&c_a;&c_g;&c_o;&c_r;&c_a;</trd>
  <trd lng="sk">mandragora</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x430;&#x43D;&#x434;&#x440;&#x430;&#x491;&#x43E;&#x440;&#x430;</trd>,
    <trd>&#x430;&#x434;&#x430;&#x43C;&#x43E;&#x432;&#x430; &#x433;&#x43E;&#x43B;&#x43E;&#x432;&#x430;</trd>
  </trdgrp>
</drv>
