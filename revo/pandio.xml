<vortaro>
<art mrk="$Id$">
<kap>
<rad>pandion</rad>/o
</kap>
<drv mrk="pandio.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="pandio.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Pandion</trd>), kaptanta fi&scirc;ojn.
    </dif>
    <ref tip="sin" cel="agl.fisx0o">fi&scirc;aglo</ref>
  </snc>
  <trd lng="cs">orel &rcaron;i&ccaron;ni</trd>
  <trd lng="de">Fischadler</trd>
  <trd lng="en">osprey</trd>
  <trd lng="hu">hal&aacute;szsas</trd>
  <trd lng="pl">rybo&lstroke;&oacute;w</trd>
  <trd lng="ru">&c_s;&c_k;&c_o;&c_p;&c_a;</trd>
</drv>
<drv mrk="pandio.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="bird.rabo0oj">rabobirdoj</ref>
      (<trd lng="la">Pandionidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Fischadler <klr tip="ind">(Familie)</klr></trd>
  <trd lng="fr">pcheur</trd>
</drv>
</art>
