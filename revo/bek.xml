<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bek.xml,v 1.47 2023/10/29 19:11:01 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bek</rad>/o
</kap>
<drv mrk="bek.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bek.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Anta&ubreve;a pinta korna ekstrema&jcirc;o de la
      <ref tip="super" cel="busx.0o">bu&scirc;o</ref> de
      la <ref tip="malprt" cel="bird.0o">birdoj</ref>:
      <ekz>
        birdo kantas la&ubreve; sia <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        &gcirc;i purigis sin per la <tld/>o kaj perdis &ccirc;e tio unu plumon
        <fnt><bib>Fab3</bib>, <lok>Estas tute certe</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ne laboru tiel sen&ccirc;ese per via babilema <tld/>o tio estas
        malsaniga por 
        la brusto  <klr>(&ccirc;esu babiladi)</klr>
        <fnt><bib>Fab4</bib>, <lok>Filino de la mar&ccirc;a
        re&gcirc;o</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="ornito1.0o">ornitorinko</ref>
    <trd lng="be">&c_d;&c_z;&c_ju;&c_b;&c_a;</trd>
    <trd lng="br">pigos</trd>
    <trd lng="de">Schnabel</trd>
    <trd lng="hu">cs&odblac;r</trd>
    <trdgrp lng="nl">
      <trd>bek<klr tip="ind"> (v.vogels)</klr></trd>,
      <trd>snavel</trd>
    </trdgrp>
    <trd lng="pl">dzi&oacute;b</trd>
    <trd lng="pt">bico</trd>
    <trd lng="ru">&c_k;&c_l;&c_ju;&c_v;</trd>
    <trd lng="sv">n&auml;bb</trd>
    <trdgrp lng="zh">
    <trd>&#x677E;&#x53E3; <pr>s&#x14D;ngk&#x1D2;u</pr></trd>,
    <trd>&#x5599; <pr>hu&#xEC;</pr></trd>,
    <trd>&#x9CE5;&#x5634; <pr>ni&#x1CE;ozu&#x1D0;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="bek.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <tld lit="B"/>simila ekstrema&jcirc;o de kelkaj objektoj:
      <ekz>
        <ind><tld/>o de skribplumo</ind>;
        <trd lng="de">Federkiel</trd>
      </ekz>
      <ekz>
        <ind><tld/>o de kru&ccirc;o</ind>;
        <trd lng="de">T&uuml;lle</trd>
      </ekz>
      <ekz>
        <ind><tld/>o de fluto</ind>;
        <trd lng="de">Mundst&uuml;ck</trd>
      </ekz>
      <ekz>
        <ind><tld/>o de &scirc;uo</ind>;
        <trd lng="de">Schuhspitze</trd>
      </ekz>
      <ekz>
        <ref cel="gas.0beko">gas<tld/>o</ref>
        <klr>(flam<tld/>o, flamingo, brulilo)</klr>.
      </ekz>
    </dif>
    <ref tip="sin" cel="ajut.0o">ajuto</ref>
    <trdgrp lng="be">
      <trd>&c_n;&c_o;&c_s;&c_ib;&c_k;</trd>,
      <trd>&c_n;&c_a;&c_k;&c_a;&c_n;&c_je;&c_ch;&c_n;&c_ib;&c_k;</trd>,
      <trd>&c_m;&c_u;&c_sh;&c_t;&c_u;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>cs&odblac;r</trd>,
      <trd>hegy<klr tip="ind">(t&aacute;rgy&eacute;)</klr></trd>
    </trdgrp>
    <trd lng="nl">bek</trd>
    <trdgrp lng="ru">
      <trd>&c_n;&c_o;&c_s;&c_i;&c_k;</trd>,
      <trd>&c_n;&c_a;&c_k;&c_o;&c_n;&c_je;&c_ch;&c_n;&c_i;&c_k;</trd>,
      <trd>&c_m;&c_u;&c_n;&c_d;&c_sh;&c_t;&c_u;&c_k;</trd>
    </trdgrp>
  </snc>
  <trd lng="br">beg</trd>
  <trd lng="cs">zob&aacute;k</trd>
  <trd lng="en">beak</trd>
  <trd lng="es">pico</trd>
  <trd lng="fr">bec</trd>
  <trd lng="he">&#x5DE;&#x5E7;&#x5D5;&#x5E8; (&#x5E9;&#x5DC; &#x5E2;&#x5D5;&#x5E3;)</trd>
  <trd lng="ja">&#x304F;&#x3061;&#x3070;&#x3057;</trd>
  <trd lng="sk">zob&aacute;k</trd>
  <trd lng="uk">&c_d;&c_z;&#x44C;&#x43E;&c_b;</trd>
</drv>

<drv mrk="bek.0i">
  <kap><tld/>i,
  <var><kap><tld/>umi</kap></var></kap>
  <snc mrk="bek.0i.bekpiki">
    <gra><vspec>tr</vspec></gra>
    <dif>
      Piki per <tld/>o:
      <ekz>
        ni toleru, ke ili <tld/>u grajnojn
        <fnt><bib>Fab2</bib>, <lok>Najbaraj familioj</lok></fnt>;
      </ekz>
      <ekz>
        li flugis malsupren en la herbon <klr>...</klr> kaj <tld/>is la
        balanci&gcirc;antajn trunketojn de la herboj
        <fnt><bib>Fab1</bib>, <lok>Galo&scirc;oj de feli&ccirc;o</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;e la ligna trogo <klr>...</klr> la novnaskitaj kokidoj
        kutimas <tld/>umi
        <fnt>
          <aut>J. Dorosmai, trad. J. Dorosmai jun. kaj J. Horvath</aut>
          <vrk>Fabloj kaj Aforismoj</vrk>
          <lok>2002</lok>
          </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_d;&c_z;&c_ja;&c_w;&c_b;&c_c;&c_ib;</trd>
    <trdgrp lng="br">
      <trd>begata</trd>,
      <trd>pigosat</trd>
    </trdgrp>
    <trd lng="de">picken</trd>
    <trd lng="en">peck</trd>
    <trdgrp lng="es">
      <trd>picar<klr tip="ind"> (ave, con el pico)</klr></trd>,
      <trd>picotear</trd>
    </trdgrp>
    <trd lng="fr">becqueter</trd>
    <trdgrp lng="hu">
      <trd>felcs&iacute;p<klr tip="ind">
      (cs&odblac;r&eacute;vel)</klr></trd>,
      <trd>csipeget</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>pikken</trd>,
      <trd>oppikken</trd>
    </trdgrp>
    <trd lng="pl">dzioba&cacute;</trd>
    <trd lng="ru">&c_k;&c_l;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
    <trd lng="sk">zoba&tcaron;</trd>
  </snc>
  <snc>
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Ion fari per la beko:
      <ekz>
        se birdo tro <tld/>as <klr>(kantas)</klr>, la katon &gcirc;i
        vekas<fnt><bib>PrV</bib></fnt>.
      </ekz>
      <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; &c_n;&c_je;&c_sh;&c_t;&c_a; <ind>&c_d;&c_z;&c_ju;&c_b;&c_a;&c_j;</ind></trd>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>kr&aacute;korat <klr>(o pt&aacute;c&iacute;ch i p&rcaron;en.)</klr></trd>,
    <trd>zp&iacute;vat</trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5E0;&#x5E7;&#x5E8;</trd>
  <trdgrp lng="ja">
    <trd>&#x3064;&#x3064;&#x304F;</trd>,
    <trd>&#x3055;&#x3048;&#x305A;&#x308B;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>spieva&tcaron;</trd>,
    <trd>&dcaron;oba&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43B;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_d;&c_z;&#x44C;&#x43E;&c_b;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="bek.kun0igxi">
  <kap>kun<tld/>i&gcirc;i</kap>
  <snc mrk="bek.kun0igxi.ZOO">
    <dif>
      Sin reciproke piki per <tld/>o.
    </dif>
    <trd lng="be">&c_d;&c_z;&c_ja;&c_w;&c_b;&c_c;&c_ib;&c_s;&c_ja;</trd>
    <trd lng="de">schn&auml;beln</trd>
    <trd lng="es">picotearse<klr tip="amb"> (uno al otro)</klr></trd>
    <trdgrp lng="fr">
      <trd>se <ind>becqueter</ind><klr> (en p. d'oiseaux)</klr></trd>,
      <trd>se donner des coups de <ind>bec</ind></trd>
    </trdgrp>
    <trd lng="hu">csipkedik egym&aacute;st</trd>
    <trd lng="nl">bekken<klr tip="ind"> (v.vogels)</klr></trd>
    <trd lng="pl">dzioba&cacute; si&eogonek;</trd>
    <trd lng="ru">&c_k;&c_l;&c_je;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;<klr tip="amb"> (&c_m;&c_je;&c_zh;&c_d;&c_u; &c_s;&c_o;&c_b;&c_o;&c_j;)</klr></trd>
  </snc>
  <snc mrk="bek.kun0igxi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Interkvereleti.
    </dif>
    <trd lng="be">&c_p;&c_ib;&c_k;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a; <klr tip="amb">(&c_s;&c_v;&c_a;&c_r;&c_y;&c_c;&c_c;&c_a;)</klr></trd>
    <trdgrp lng="fr">
      <trd>se prendre le <ind>bec</ind></trd>,
      <trd>se <ind>quereller</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>civakodnak</trd>,
      <trd>csipkel&odblac;dnek</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>bekken<klr tip="ind"> (snauwen)</klr></trd>,
      <trd>snauwen</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_i;&c_k;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;<klr tip="amb"> (&c_s;&c_s;&c_o;&c_r;&c_i;&c_t;&c_mol;&c_s;&c_ja;)</klr></trd>
  </snc>
  <trd lng="br">en em <ind>vegata</ind></trd>
  <trd lng="en">peck</trd>
  <trd lng="ja">&#x3064;&#x3064;&#x304D;&#x3042;&#x3046;</trd>
</drv>

<drv mrk="bek.cikonio0o">
  <kap>cikonio<tld/>o</kap>
  <snc mrk="bek.cikonio0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <ref tip="dif" cel="gerani.0o">geranio</ref>
    <rim mrk="bek.cikonio0o.RIM">
      Tiel nomata pro la formo de la frukto.
    </rim>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_r;&c_a;&c_n;&c_mol;</trd>
  <trd lng="br">geraniom</trd>
  <trd lng="es">geranio</trd>
  <trd lng="hu">g&oacute;lyaorr</trd>
  <trd lng="nl">ooievaarsbek<klr tip="ind"> (geranium)</klr></trd>
  <trd lng="pl">geranium</trd>
  <trd lng="ru">&c_g;&c_je;&c_r;&c_a;&c_n;&c_mol;</trd>
  <adm>
    &Ccirc;u la formo &leftquot;cikonibeko&rightquot; ne estus preferinda?
    <aut>MB</aut>
  </adm>
</drv>

<drv mrk="bek.cxapo0o">
  <kap>&ccirc;apo<tld/>o</kap>
  <snc mrk="bek.cxapo0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Elstara anta&ubreve;a&jcirc;o de <ref tip="malprt" cel="cxap.0o.TEKS">&ccirc;apo</ref>,
      &scirc;irmanta la okulojn.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="vizier.0o">viziero</ref>
  </refgrp>
  <trd lng="be">&c_b;&c_r;&c_y;&c_l;&c_mol;</trd>
  <trd lng="br">lerbenn</trd>
  <trd lng="de">Schild</trd>
  <trd lng="en">visor</trd>
  <trd lng="es">visera</trd>
  <trd lng="fr">visi&egrave;re<klr tip="ind"> (d'une casquette)</klr></trd>
  <trdgrp lng="hu">
    <trd>sapkaellenz&odblac;</trd>,
    <trd>sild</trd>
  </trdgrp>
  <trd lng="ja">&#x3072;&#x3055;&#x3057;</trd>
  <trd lng="nl">vizier</trd>
  <trd lng="pl">daszek czapki</trd>
  <trd lng="ru">&c_k;&c_o;&c_z;&c_y;&c_r;&c_jo;&c_k;</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_a;&c_sh;&#x43E;&#x43A;</trd>,
    <trd>&#x43A;&#x43E;&c_z;&c_i;&c_r;&#x43E;&#x43A;</trd>
  </trdgrp>
</drv>

<drv mrk="bek.flav0ulo">
  <kap>flav<tld/>ulo</kap>
  <snc>
    <dif>
      Juna senspertula&ccirc;o:
      <ekz>
        kiu estas &ccirc;i tiu flav<tld/>ulo
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 12a</lok></fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_w;&c_t;&c_a;&c_r;&c_o;&c_c;&c_ib;&c_k;</trd>
  <trd lng="br">beg-melen</trd>
  <trdgrp lng="cs">
    <trd>grenhorn</trd>,
    <trd>holobr&aacute;dek</trd>,
    <trd>usmrkanec</trd>,
    <trd>zelen&aacute;&ccaron;</trd>
  </trdgrp>
  <trd lng="de">Gr&uuml;nschnabel</trd>
  <trd lng="en">greenhorn</trd>
  <trdgrp lng="es">
    <trd>novato</trd>,
    <trd>pipiolo</trd>,
    <trd>mocoso<klr tip="ind"> (inexperto)</klr></trd>
  </trdgrp>
  <trd lng="fr">blanc bec</trd>
  <trd lng="hu">z&ouml;ldf&uuml;l&udblac;</trd>
  <trdgrp lng="ja">
    <trd>&#x9752;&#x4E8C;&#x624D;  <pr>&#x3042;&#x304A;&#x306B;&#x3055;&#x3044;</pr></trd>,
    <trd>&#x3072;&#x3088;&#x3063;&#x3053;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>melkmuil</trd>,
    <trd>vlasbaard<klr tip="ind"> (melkmuil)</klr></trd>
  </trdgrp>
  <trd lng="pl">&zdot;&oacute;&lstroke;todzi&oacute;b</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_t;&c_o;&c_r;&c_o;&c_t;&c_y;&c_j; &c_ju;&c_n;&c_je;&c_c;</trd>
  <trdgrp lng="sk">
    <trd>nesk&uacute;sen&yacute; &ccaron;lovek</trd>,
    <trd>za&ccaron;iato&ccaron;n&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_zh;&#x43E;&c_v;&c_t;&#x43E;&c_r;&#x43E;&c_t;&c_i;&c_j; (&c_u; &c_z;&#x43D;&c_a;&c_ch;. &c_ib;&#x43C;.)</trd>,
    <trd>&#x43C;&#x43E;&#x43B;&#x43E;&#x43A;&#x43E;&c_s;&#x43E;&c_s;</trd>
  </trdgrp>
</drv>

<drv mrk="bek.kruc0ulo">
  <kap>kruc<tld/>ulo</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="bek.kruc0ulo.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="loksi.0o.ZOO">loksio</ref>
    <trd lng="zh">&#x9DA1; <pr>h&#xE9;</pr></trd>
</snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_zh;&c_a;&c_d;&c_z;&c_ju;&c_b;</trd>
  <trd lng="br">beg-kroaz</trd>
  <trd lng="cs">k&rcaron;ivka</trd>
  <trd lng="de">Kreuzschnabel</trd>
  <trdgrp lng="en">
    <trd>crossbill</trd>
  </trdgrp>
  <trd lng="es">piquituerto</trd>
  <trdgrp lng="fr">
    <trd>bec-crois&eacute;</trd>
  </trdgrp>
  <trd lng="hu">keresztcs&odblac;r&udblac;</trd>
  <trd lng="nl">kruisbek</trd>
  <trd lng="pl">krzy&zdot;odzi&oacute;b</trd>
  <trd lng="ru">&c_k;&c_l;&c_jo;&c_s;&c_t;</trd>
  <trd lng="sk">krivonos</trd>
  <trd lng="uk">&c_sh;&c_i;&c_sh;&#x43A;&c_a;&c_r;</trd>
</drv>

<drv mrk="bek.sxu0ulo">
  <kap>&scirc;u<tld/>ulo</kap>
  <snc mrk="bek.sxu0ulo.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:zoologiaj_specioj">Specio</ref> de 
      <ref tip="lst" lst="voko:birdoj" cel="bird.0o">birdo</ref> (<trd lng="la">Balaeniceps
      rex</trd>),
      granda cikonieca birdo vivanta en tropika orienta Afriko:
      <ekz>
        la saharaj inunditaj herbejoj gastigas la &ccirc;efajn populaciojn de
        antilopoj de Afriko, same kiel diversajn speciojn de birdoj,
        kiel la &scirc;u<tld/>ulo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="&Viki;/Saharaj_inunditaj_herbejoj">Saharaj
          inunditaj herbejoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/3/3c/Shoebill_(su_neko).jpg/320px-Shoebill_(su_neko).jpg" prm="CC BY 2.0">
      &Scirc;u<tld/>ulo (Balaeniceps rex)
      <fnt>
        <aut>su neko, propra verko</aut>
        <lok><url ref="&WCW;/File:Shoebill_(su_neko).jpg">&WCW;/File:Shoebill_(su_neko).jpg</url></lok>
      </fnt>
    </bld>
  </snc>
  <trd lng="af">skoenbekooievaar</trd>
  <trd lng="ca">bec d'esclop</trd>
  <trd lng="de">Schuhschnabel</trd>
  <trd lng="en">shoebill</trd>
  <trd lng="es">picozapato</trd>
  <trd lng="it">becco a scarpa</trd>
  <trd lng="nl">schoenbekooievaar</trd>
  <trd lng="ru">&c_k;&c_i;&c_t;&c_o;&c_g;&c_l;&c_a;&c_v;</trd>
  <trd lng="sv">tr&auml;skon&auml;bb</trd>
</drv>
</art>
