<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>bek</rad>/o
</kap>
<drv mrk="bek.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Anta&ubreve;a pinta korna ekstrema&jcirc;o de la
      <ref tip="super" cel="busx.0o">bu&scirc;o</ref> de
      la <ref tip="malprt" cel="bird.0o">birdoj</ref>.
    </dif>
    <trd lng="de">Schnabel</trd>
    <trd lng="ru">&c_k;&c_l;&c_ju;&c_v;</trd>
  </snc>
  <snc>
    <uzo tip="klr">(f)</uzo>
    <dif>
      <tld/>simila ekstrema&jcirc;o de kelkaj objektoj:
      <ekz>
        <tld/>o de skribplumo, gas<tld/>o <klr>(flam<tld/>o, flamingo,
        brulilo)</klr>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_n;&c_o;&c_s;&c_i;&c_k;</trd>,
      <trd>&c_n;&c_a;&c_k;&c_o;&c_n;&c_je;&c_ch;&c_n;&c_i;&c_k;</trd>,
      <trd>&c_m;&c_u;&c_n;&c_d;&c_sh;&c_t;&c_u;&c_k;</trd>
    </trdgrp>
  </snc>
</drv>
<drv mrk="bek.0i">
  <kap><tld/>i</kap>
  <snc>
    <gra><vspec>tr</vspec></gra>
    <dif>
      Piki per <tld/>o:
      <ekz>
        <tld/>i grajnojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/> la trunketojn de la herboj
        <fnt>(<aut>Zamenhof</aut>: <vrk>Fabeloj de Andersen,
        Galo&scirc;oj de feli&ccirc;o</vrk>)</fnt>.
      </ekz>
    </dif>
    <trd lng="de">picken</trd>
    <trd lng="ru">&c_k;&c_l;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Kanti:
      <ekz>
        se birdo tro <tld/>as, la katon &gcirc;i vekas
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>.
      </ekz>
    </dif>
  </snc>
</drv>
<drv mrk="bek.kun0igxi">
  <kap>kun<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Sin reciproke piki per <tld/>o.
    </dif>
    <trd lng="de">schn&auml;beln</trd>
  </snc>
  <snc>
    <uzo tip="klr">(f)</uzo>
    <dif>
      Interkvereli.
    </dif>
  </snc>
</drv>
<drv mrk="bek.cikonio0o">
  <kap>cikonio<tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="gerani.0o">Geranio</ref>
      <klr>(pro la formo de la frukto)</klr>.
    </dif>
  </snc>
</drv>
<drv mrk="bek.cxapo0o">
  <kap>&ccirc;apo<tld/>o</kap>
  <snc>
    <dif>
      Elstara anta&ubreve;a&jcirc;o de &ccirc;apo, &scirc;irmanta la okulojn;
      viziero.
    </dif>
  </snc>
  <trd>Schild</trd>
  <trd lng="ru">&c_k;&c_o;&c_z;&c_y;&c_r;&#1105;&c_k;</trd>
</drv>
<drv mrk="bek.flav0ulo">
  <kap>flav<tld/>ulo<fnt>B</fnt></kap>
  <snc>
    <dif>
      Juna senspertula&ccirc;o.
    </dif>
  </snc>
  <trd lng="de">Gr&uuml;nschnabel</trd>
  <trd lng="en">greenhorn</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_t;&c_o;&c_r;&c_o;&c_t;&c_y;&c_j;
  &c_ju;&c_n;&c_je;&c_c;</trd>
</drv>
</art>
