<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>bek</rad>/o
</kap>
<drv mrk="bek.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bek.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Anta&ubreve;a pinta korna ekstrema&jcirc;o de la
      <ref tip="super" cel="busx.0o">bu&scirc;o</ref> de
      la <ref tip="malprt" cel="bird.0o">birdoj</ref>.
    </dif>
    <trd lng="be">&c_d;&c_z;&c_ju;&c_b;&c_a;</trd>
    <trd lng="br">pigos</trd>
    <trd lng="de">Schnabel</trd>
    <trd lng="hu">cs&odblac;r</trd>
    <trdgrp lng="nl">
      <trd>bek<klr tip="ind"> (v.vogels)</klr></trd>,
      <trd>snavel</trd>
    </trdgrp>
    <trd lng="pl">dzi&oacute;b</trd>
    <trd lng="ru">&c_k;&c_l;&c_ju;&c_v;</trd>
  </snc>
  <snc mrk="bek.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <tld lit="B"/>simila ekstrema&jcirc;o de kelkaj objektoj:
      <ekz>
        <tld/>o de skribplumo;
      </ekz>
      <ekz>
        <ref cel="gas.0beko">gas<tld/>o</ref>
        <klr>(flam<tld/>o, flamingo, brulilo)</klr>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_o;&c_s;&c_ib;&c_k;</trd>,
      <trd>&c_n;&c_a;&c_k;&c_a;&c_n;&c_je;&c_ch;&c_n;&c_ib;&c_k;</trd>,
      <trd>&c_m;&c_u;&c_sh;&c_t;&c_u;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>cs&odblac;r</trd>,
      <trd>hegy<klr tip="ind">(t&aacute;rgy&eacute;)</klr></trd>
    </trdgrp>
    <trd lng="nl">bek</trd>
    <trdgrp lng="ru">
      <trd>&c_n;&c_o;&c_s;&c_i;&c_k;</trd>,
      <trd>&c_n;&c_a;&c_k;&c_o;&c_n;&c_je;&c_ch;&c_n;&c_i;&c_k;</trd>,
      <trd>&c_m;&c_u;&c_n;&c_d;&c_sh;&c_t;&c_u;&c_k;</trd>
    </trdgrp>
  </snc>
  <trd lng="br">beg</trd>
  <trd lng="en">beak</trd>
  <trd lng="fr">bec</trd>
  <trd lng="sk">zob&aacute;k</trd>
</drv>

<drv mrk="bek.0i">
  <kap><tld/>i</kap>
  <snc mrk="bek.0i.bekpiki">
    <gra><vspec>tr</vspec></gra>
    <dif>
      Piki per <tld/>o:
      <ekz>
        ni toleru, ke ili <tld/>u grajnojn
        <fnt><bib>Fab2</bib>, <lok>najbaraj familioj</lok></fnt>;
      </ekz>
      <ekz>
        li flugis malsupren en la herbon <klr>...</klr> kaj <tld/>is la
        balanci&gcirc;antajn trunketojn de la herboj
        <fnt><bib>Fab1</bib>, <lok>galo&scirc;oj de feli&ccirc;o, 5, metamorfozo de la
        skribisto</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_d;&c_z;&c_ja;&c_w;&c_b;&c_c;&c_ib;</trd>
    <trdgrp lng="br">
      <trd>begata</trd>,
      <trd>pigosat</trd>
    </trdgrp>
    <trd lng="de">picken</trd>
    <trd lng="en">peck</trd>
    <trd lng="fr">becqueter</trd>
    <trdgrp lng="hu">
      <trd>felcs&iacute;p<klr tip="ind">
      (cs&odblac;r&eacute;vel)</klr></trd>,
      <trd>csipeget</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>pikken</trd>,
      <trd>oppikken</trd>
    </trdgrp>
    <trd lng="pl">dzioba&cacute;</trd>
    <trd lng="ru">&c_k;&c_l;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
    <trd lng="sk">zoba&tcaron;</trd>
  </snc>
  <snc>
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Ion fari per la beko:
      <ekz>
        se birdo tro <tld/>as <klr>(kantas)</klr>, la katon &gcirc;i
        vekas<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
</drv>

<drv mrk="bek.kun0igxi">
  <kap>kun<tld/>i&gcirc;i</kap>
  <snc mrk="bek.kun0igxi.ZOO">
    <dif>
      Sin reciproke piki per <tld/>o.
    </dif>
    <trd lng="be">&c_d;&c_z;&c_ja;&c_w;&c_b;&c_c;&c_ib;&c_s;&c_ja;</trd>
    <trd lng="de">schn&auml;beln</trd>
    <trdgrp lng="fr">
      <trd>se <ind>becqueter</ind><klr> (en p. d'oiseaux)</klr></trd>,
      <trd>se donner des coups de <ind>bec</ind></trd>
    </trdgrp>
    <trd lng="hu">csipkedik egym&aacute;st</trd>
    <trd lng="nl">bekken<klr tip="ind"> (v.vogels)</klr></trd>
    <trd lng="pl">dzioba&cacute; si&eogonek;</trd>
    <trd lng="ru">&c_k;&c_l;&c_je;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;<klr tip="amb"> (&c_m;&c_je;&c_zh;&c_d;&c_u; &c_s;&c_o;&c_b;&c_o;&c_j;)</klr></trd>
  </snc>
  <snc mrk="bek.kun0igxi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Interkvereleti.
    </dif>
    <trd lng="be">&c_p;&c_ib;&c_k;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a; <klr tip="amb">(&c_s;&c_v;&c_a;&c_r;&c_y;&c_c;&c_c;&c_a;)</klr></trd>
    <trdgrp lng="fr">
      <trd>se prendre le <ind>bec</ind></trd>,
      <trd>se <ind>quereller</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>civakodnak</trd>,
      <trd>csipkel&odblac;dnek</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>bekken<klr tip="ind"> (snauwen)</klr></trd>,
      <trd>snauwen</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_i;&c_k;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;<klr tip="amb"> (&c_s;&c_s;&c_o;&c_r;&c_i;&c_t;&c_mol;&c_s;&c_ja;)</klr></trd>
  </snc>
  <trd lng="br">en em <ind>vegata</ind></trd>
  <trd lng="en">peck</trd>
</drv>

<drv mrk="bek.cikonio0o">
  <kap>cikonio<tld/>o</kap>
  <snc mrk="bek.cikonio0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <ref tip="dif" cel="gerani.0o">geranio</ref>
    <rim mrk="bek.cikonio0o.RIM">
      Tiel nomata pro la formo de la frukto.
    </rim>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_r;&c_a;&c_n;&c_mol;</trd>
  <trd lng="br">geraniom</trd>
  <trd lng="hu">g&oacute;lyaorr</trd>
  <trd lng="nl">ooievaarsbek<klr tip="ind"> (geranium)</klr></trd>
  <trd lng="pl">geranium</trd>
  <trd lng="ru">&c_g;&c_je;&c_r;&c_a;&c_n;&c_mol;</trd>
  <adm>
    &Ccirc;u la formo &leftquot;cikonibeko&rightquot; ne estus preferinda?
    <aut>MB</aut>
  </adm>
</drv>

<drv mrk="bek.cxapo0o">
  <kap>&ccirc;apo<tld/>o</kap>
  <snc mrk="bek.cxapo0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Elstara anta&ubreve;a&jcirc;o de <ref tip="malprt" cel="cxap.0o.TEKS">&ccirc;apo</ref>,
      &scirc;irmanta la okulojn.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="vizier.0o">viziero</ref>
  </refgrp>
  <trd lng="be">&c_b;&c_r;&c_y;&c_l;&c_mol;</trd>
  <trd lng="br">lerbenn</trd>
  <trd lng="de">Schild</trd>
  <trd lng="en">visor</trd>
  <trd lng="fr">visi&egrave;re<klr tip="ind"> (d'une casquette)</klr></trd>
  <trdgrp lng="hu">
    <trd>sapkaellenz&odblac;</trd>,
    <trd>sild</trd>
  </trdgrp>
  <trd lng="nl">vizier</trd>
  <trd lng="pl">daszek czapki</trd>
  <trd lng="ru">&c_k;&c_o;&c_z;&c_y;&c_r;&c_jo;&c_k;</trd>
</drv>

<drv mrk="bek.flav0ulo">
  <kap>flav<tld/>ulo</kap>
  <snc>
    <dif>
      Juna senspertula&ccirc;o:
      <ekz>
        kiu estas &ccirc;i tiu flav<tld/>ulo
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 12a</lok></fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="br">beg-melen</trd>
  <trd lng="de">Gr&uuml;nschnabel</trd>
  <trd lng="en">greenhorn</trd>
  <trd lng="fr">blanc bec</trd>
  <trd lng="hu">z&ouml;ldf&uuml;l&udblac;</trd>
  <trdgrp lng="nl">
    <trd>melkmuil</trd>,
    <trd>vlasbaard<klr tip="ind"> (melkmuil)</klr></trd>
  </trdgrp>
  <trd lng="pl">&zdot;&oacute;&lstroke;todzi&oacute;b</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_t;&c_o;&c_r;&c_o;&c_t;&c_y;&c_j; &c_ju;&c_n;&c_je;&c_c;</trd>
</drv>

<drv mrk="bek.kruc0ulo">
  <kap>kruc<tld/>ulo</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="bek.kruc0ulo.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="loksi.0o.ZOO">loksio</ref>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_zh;&c_a;&c_d;&c_z;&c_ju;&c_b;</trd>
  <trd lng="br">beg-kroaz</trd>
  <trd lng="de">Kreuzschnabel</trd>
  <trdgrp lng="en">
    <trd>crossbill</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bec-crois&eacute;</trd>
  </trdgrp>
  <trd lng="hu">keresztcs&odblac;r&udblac;</trd>
  <trd lng="nl">kruisbek</trd>
  <trd lng="pl">krzy&zdot;odzi&oacute;b</trd>
  <trd lng="ru">&c_k;&c_l;&c_jo;&c_s;&c_t;</trd>
</drv>
</art>
