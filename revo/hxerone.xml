<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
   <art mrk="$Id: hxerone.xml,v 1.12 2023/07/30 18:28:37 revo Exp $">
     <kap><rad>&Hcirc;erone</rad>/o <fnt><bib>NePiv</bib></fnt></kap>

<drv mrk="hxerone.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Antikva <ref tip="lst" cel="urb.0o" lst="voko:urboj">urbo</ref> 
      en <ref tip="malprt" cel="grek.G0ujo">Grekujo</ref>, 
      apud kiu okazis du gravaj bataloj
      de la antikva historio; la naski&gcirc;loko de Plutarko:
      <ekz>
        &leftquot;De Trojo &gcirc;is <tld/>o &ndash; evoluo de la greka militarto&rightquot;
        <fnt>
          <vrk><url ref="http://pola-retradio.org/6489-2/pago-4/">Hoplitoj.
         Pri la militarto de la antikva Grekio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_H;&c_je;&c_r;&c_o;&c_n;&c_e;&c_ja;</trd>
   <trd lng="bg">&c_H;&c_je;&c_r;&c_o;&c_n;&c_je;&c_ja;</trd>
   <trd lng="de">Chaironeia</trd>
   <trd lng="en">Chaeronea</trd>
   <trd lng="fr">Ch&eacute;ron&eacute;e</trd>
   <trd lng="el">&Chi;&alfa;&jota;&rho;&omega_acute;&ny;&epsilon;&jota;&alfa;</trd>
   <trd lng="fi">Khaironeia</trd>
   <trd lng="la">Chaeronea</trd>
   <trd lng="pl">Cheronea</trd>
   <trd lng="pt">Queron&eacute;ia</trd>
   <trd lng="ru">&c_H;&c_je;&c_r;&c_o;&c_n;&c_je;&c_ja;</trd>
   <trd lng="it">Cheronea</trd>
 </drv>
 </art>
