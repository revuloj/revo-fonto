<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gitar.xml,v 1.31 2023/11/11 11:36:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>gitar</rad>/o
</kap>
<drv mrk="gitar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gitar.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Hispandevena <ref tip="lst" cel="instru2.kord0o.MUZ" lst="voko:kordinstrumentoj">kordinstrumento</ref>
      ordinare seskorda, kiun oni sonigas pin&ccirc;e a&ubreve; plektre:
      <ekz>
        mi lernas pentri kaj ludi <tld/>on
        <fnt>
          <bib>DL</bib>
          <lok>Dua Libro de l' Lingvo Internacia</lok>
        </fnt>;
      </ekz>
      <ekz>
        Ni tie sur suproj, &ccirc;e &scirc;tonaj ruinoj &parsl; Sonigos mal&gcirc;ojajn <tld/>ojn &parsl; Kaj kun la
        plorantaj eksonoj sonoraj &parsl; Ni kantos pri miaj animaj doloroj
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la <klr>[&#x2026;]</klr> muro pendis <tld/>o
        <fnt><bib>InfanTorent2</bib>, <lok>unua parto, &ccirc;apitro
        1a</lok></fnt>;
      </ekz>
      <ekz>
        <nom>Zoja</nom> sin klinas super la <tld/>o kaj stre&ccirc;igas la malforti&gcirc;intajn kordojn
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/9/96/Carlo-domeniconi_photo_dj-17062009-103.jpg/320px-Carlo-domeniconi_photo_dj-17062009-103.jpg" prm="CC BY-SA 3.0">Carlo Domeniconi kun <tld/>o
        <fnt>
          <aut>Davidjohnberlin, propra verko</aut>
          <lok><url ref="&WCW;/File:Carlo-domeniconi_photo_dj-17062009-103.jpg">https://commons.wikimedia.org/wiki/File:Carlo-domeniconi_photo_dj-17062009-103.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_t;&c_a;&c_r;&c_a;</trd>
  <trd lng="bo">&t_sa;&t_gata;&t_rata;&t_tsheg;&t_sa;&t_nyata;&t_na;&t_tsheg;</trd>
  <trd lng="ca">guitarra</trd>
  <trd lng="cs">kytara</trd>
  <trdgrp lng="de">
    <trd>Gitarre</trd>,
    <trd>Klampfe</trd>
  </trdgrp>
  <trd lng="en">guitar</trd>
  <trd lng="es">guitarra</trd>
  <trd lng="fa">&f_g;&f_y;&a_t;&a_A;&a_r;</trd>
  <trd lng="fr">guitare</trd>
  <trd lng="he">&gimel;&yod;&tet;&resh;&he;</trd>
  <trd lng="hu">git&aacute;r</trd>
  <trd lng="id">gitar</trd>
  <trd lng="ja">&#x30AE;&#x30BF;&#x30FC;</trd>
  <trd lng="nl">gitaar</trd>
  <trd lng="pl">gitara</trd>
  <trdgrp lng="pt">
   <trd>viol&atilde;o</trd>, 
   <trd>viola</trd> 
  </trdgrp>
  <trd lng="ru">&c_g;&c_i;&c_t;&c_a;&c_r;&c_a;</trd>
  <trd lng="sk">gitara</trd>
  <trd lng="sv">gitarr</trd>
  <trd lng="tr">gitar</trd>
  <trd lng="uk">&c_g;&c_ib;&c_t;&c_a;&c_r;&c_a;</trd>
  <trd lng="zh">&#x5409;&#x4ED6; <pr>j&#xED;t&#x101;</pr></trd>
</drv>
</art>
