<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>gitar</rad>/o
</kap>
<drv mrk="gitar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gitar.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Hispandevena <ref tip="lst" cel="instru2.kord0o.MUZ"
      lst="voko:kordinstrumentoj">kordinstrumento</ref>
      ordinare seskorda, kiun oni sonigas pin&ccirc;e a&ubreve; plektre:
      <ekz>
        sur la <klr>...</klr> muro pendis <tld/>o
        <fnt><bib>InfanTorent2</bib>, <lok>unua parto, &ccirc;apitro
        1a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_t;&c_a;&c_r;&c_a;</trd>
  <trd lng="ca">guitarra</trd>
  <trdgrp lng="de">
    <trd>Gitarre</trd>,
    <trd>Klampfe</trd>
  </trdgrp>
  <trd lng="en">guitar</trd>
  <trd lng="es">guitarra</trd>
  <trd lng="fa">&f_g;&f_y;&a_t;&a_A;&a_r;</trd>
  <trd lng="fr">guitare</trd>
  <trd lng="hu">git&aacute;r</trd>
  <trd lng="id">gitar</trd>
  <trd lng="nl">gitaar</trd>
  <trd lng="pl">gitara</trd>
  <trdgrp lng="pt">
   <trd>viol&atilde;o</trd>, 
   <trd>viola</trd> 
  </trdgrp>
  <trd lng="ru">&c_g;&c_i;&c_t;&c_a;&c_r;&c_a;</trd>
  <trd lng="sv">gitarr</trd>
  <trd lng="tr">gitar</trd>
</drv>
</art>
