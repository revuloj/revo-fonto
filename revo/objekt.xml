<drv mrk="objekt.0a">
  <kap><tld/>a</kap>
  <snc mrk="objekt.0a.perceptebla">
    <dif>
      En la formo de <tld/>o, t.e. perceptebla ktp:
      <ekz>
        la premioj estis monaj kaj <tld/>aj (pokaloj)
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2006/009096.php">Glaciskulptado</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_e;&c_ch;&c_a;&c_v;&c_y;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_d;&c_m;&c_je;&c_t;&c_n;&c_y;</trd>,
      <trd>&c_a;&c_b;&#8217;&c_je;&c_k;&c_t;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Sach-</trd>,
      <trd>sachlich</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_je;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
    <trdgrp lng="zh">
    <trd>&#x4E0E;&#x4E8B;&#x5B9E;&#26377;&#20851; <pr>y&#x1D4;sh&#xEC;sh&#xED;y&#x1D2;ugu&amacron;n</pr></trd>,
    <trd>&#33287;&#x4E8B;&#x5BE6;&#26377;&#x95DC; <pr>y&#x1D4;sh&#xEC;sh&#xED;y&#x1D2;ugu&amacron;n</pr></trd>,
    <trd>&#x53D7;&#x683C; <pr>sh&#xF2;ug&#xE9;</pr></trd>
  </trdgrp>
</snc>
   <snc mrk="objekt.0a.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      Rilata al <ref tip="vid" cel="objekt.0o.GRA"><tld/>o<sncref/></ref>:
      <ekz>
        <tld/>a komplemento;
      </ekz><ekz>
        <tld/>a predikativo
        <fnt>
          <bib>LR</bib>
          <lok>5. Kazosintakso</lok>
        </fnt>.
