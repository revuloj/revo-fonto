<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>veruk</rad>/o
</kap>
<drv mrk="veruk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Malgranda elkreska&jcirc;o de la ha&ubreve;to:
      <ekz>
        haro el &scirc;ia <tld/>o iom tiklis min<fnt><aut>Spomenka
        &Scirc;timec</aut>:
        <vrk>Morto en Melburna tramo</vrk>, <bib>Monato</bib><lok>jaro
        1997a, numero 8a, p. 24a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="tumor.0o.MED">tumoro</ref>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_r;&c_o;&c_d;&c_a;&c_w;&c_k;&c_a;</trd>
  <trd lng="de">Warze</trd>
  <trd lng="en">wart</trd>
  <trd lng="hu">szem&ouml;lcs</trd>
  <trd
  lng="el">&kappa;&rho;&epsilon;&alfa;&tau;&omikron;&epsilon;&lambda;&jota;&alfa_ton;</trd>
  <trdgrp lng="fr">
    <trd>verrue</trd>
  </trdgrp>
  <trd lng="nl">wrat</trd>
  <trdgrp lng="pl">
    <trd>kurzajka</trd>,
    <trd>brodawka</trd>
  </trdgrp>
  <trd lng="pt">verruga</trd>
  <trd lng="ru">&c_b;&c_o;&c_r;&c_o;&c_d;&c_a;&c_v;&c_k;&c_a;</trd>
  <trd lng="sv">v&aring;rta</trd>
</drv>
</art>
