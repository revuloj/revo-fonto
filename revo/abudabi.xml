<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: abudabi.xml,v 1.4 2023/08/27 14:47:00 revo Exp $">
<kap>
    <rad>Abudabi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="abudabi.A0o">
  <kap><tld/>o</kap>
  <snc mrk="abudabi.A0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" cel="urb.cxef0o" lst="voko:&ccirc;efurboj">&Ccirc;efurbo</ref>
      kaj emirlando de <ref tip="malprt" cel="emir.arabaj0landoj">Unui&gcirc;intaj Arabaj
      Emirlandoj</ref>:
      <ekz>
        arabaj kaj okcidentarabaj nombroj sur &scirc;osesignalo en <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=553834">Hind&ndash;araba
          nombrosistemo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estis fondita en 1760 kaj estis sidejo de lokaj regantoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=228">Abudabio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Abu Dhabi</trd>
  </snc>
  <trd lng="be">&c_A;&c_b;&c_u;-&c_D;&c_a;&c_b;&c_ib;</trd>
  <trd lng="fr">Abou Dabi</trd>
  <trd lng="ja">&#x30A2;&#x30D6;&#x30C0;&#x30D3;  [<ind>&#x3042;&#x3076;&#x3060;&#x3073;</ind>]</trd>
</drv>
</art>

