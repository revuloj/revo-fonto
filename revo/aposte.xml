<kap>
  <rad>aposterior</rad>/a
</kap>
<drv mrk="aposte.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Bazita sur faktoj kaj spertoj, ne nur sur teorio:
      <ekz>
        Esperanto estas artefarita <tld/>a lingvo.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="aprior.0a">apriora</ref>.
    </refgrp>
  </snc>
  <trd lng="be">
&c_a;&c_p;&c_a;&c_s;&c_t;&c_e;&c_r;&c_y;&c_jo;&c_r;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>aposteriorn&#xED;</trd>,
    <trd>nejen na teorii</trd>,
    <trd>zalo&#x17E;en&#xFD; na zku&scaron;enosti</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>a <ind>posteriori</ind></trd>,
     <trd>aposteriorisch</trd>,
     <trd>im Nachhinein</trd>,
     <trd>aus der <ind>Wahrnehmung</ind> gewonnen</trd>,
     <trd>im <ind>Einklang</ind> mit der Erfahrung</trd>,
     <trd>aus <ind>Erfahrung</ind></trd>
  </trdgrp>
  <trd lng="en">a posteriori</trd>
  <trd lng="es">a posteriori</trd>
  <trd lng="fr">a posteriori</trd>
  <trd lng="he">&#x5D1;&#x5D3;&#x5D9;&#x5E2;&#x5D1;&#x5D3;</trd>
  <trd lng="hu">a posteriori</trd>
  <trd lng="it">a posteriori</trd>
  <trdgrp lng="ja">
    <trd>&#x5F8C;&#22825;&#30340;  <pr>&#12371;&#12358;&#12390;&#12435;&#12390;&#x304D;</pr></trd>,
    <trd>&#x7D4C;&#x9A13;&#30340;  <pr>&#12369;&#12356;&#12369;&#12435;&#12390;&#x304D;</pr></trd>,
    <trd>&#x5E30;&#x7D0D;&#30340;  <pr>&#x304D;&#x306E;&#12358;&#12390;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="pl">a posteriori</trd>
  <trd lng="ru">
&c_a;&c_p;&c_o;&c_s;&c_t;&c_je;&c_r;&c_i;&c_o;&c_r;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>aposteri&oacute;rny</trd>,
    <trd>aposteri&#xF3;rny</trd>,
    <trd>sk&#xFA;senostn&#xFD;</trd>
  </trdgrp>
  <trd lng="uk">&c_a;&#x43F;&#x43E;&c_s;&c_t;&c_je;&c_r;&c_ib;&#x43E;&c_r;&#x43D;&c_i;&c_j;</trd>
</drv>
</art>
