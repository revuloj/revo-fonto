<kap>
  <rad>bra&hcirc;icefal</rad>/o <fnt><bib>BoER</bib></fnt>,
  <var><kap><rad>brakicefal</rad>/o <fnt><bib>PV</bib></fnt></kap></var>
</kap>
<drv mrk="brahxicefal.0o">
  <kap><tld/>o,
    <var><kap>brakicefalo</kap></var>
  </kap>
  <snc mrk="brahxicefal.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Homo kies krania lar&gcirc;o egalas al &gcirc;ia longo:
      <ekz>
        por fabelo eble ne tre gravas precizeco de terminoj &mdash; 
        tamen atentu, ke <tld/>o tute ne egalas anacefalon
        <fnt>
          <vrk><url ref="http://trovu.com/plumo/lundo_sabato">Inter 
          lundo kaj sabato | Verda plumo: literaturaj konkursoj ...</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="dolihxocefal.0o.ANA">doli&hcirc;ocefalo</ref>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_a;&c_h;&c_ib;&c_c;&c_e;&c_f;&c_a;&c_l;</trd>
  <trd lng="ca">braquic&egrave;fal</trd>
  <trdgrp lng="de">
    <trd>Brachyzephale</trd>,
    <trd>Kurzkopf</trd>
  </trdgrp>
  <trd lng="en">brachycephalic person <klr>(or animal)</klr></trd>
  <trd lng="es">braquic&eacute;falo</trd>
  <trd lng="fr">brachyc&eacute;phale<klr> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>brachycephal</trd>,
    <trd>r&ouml;vidfej&udblac; (ember)</trd>
  </trdgrp>
  <trd lng="it">brachicefalo</trd>
  <trd lng="ja">&#x77ED;&#x982D;  <pr>&#x305F;&#x3093;&#x3068;&#x3046;</pr></trd>
  <trd lng="pl">brachicefal</trd>
  <trd lng="ru">&c_b;&c_r;&c_a;&c_h;&c_i;&c_c;&c_je;&c_f;&c_a;&c_l;</trd>
  <trdgrp lng="uk">
    <trd>&#x431;&#x440;&#x430;&#x43A;&#x456;&#x446;&#x435;&#x444;&#x430;&#x43B;</trd>,
    <trd>&#x43A;&#x43E;&#x440;&#x43E;&#x442;&#x43A;&#x43E;&#x433;&#x43E;&#x43B;&#x43E;&#x432;&#x438;&#x439;</trd>
