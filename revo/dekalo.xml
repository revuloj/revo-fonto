<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>dekalog</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="dekalo.0o">
  <kap><tld/>o</kap>
  <snc mrk="dekalo.0o.BIB">
    <uzo tip="fak">BIB</uzo>
    <uzo tip="fak">KRI</uzo>
    <dif>
      La dek religiaj instruoj, kiujn la Biblio prezentas kiel
      diktitajn de Javeo al Moseo, kaj kiuj i&gcirc;is baza le&gcirc;o de la
      hebrea popolo.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_e;&c_k;&c_a;&c_l;&c_jo;&c_g;</trd>,
    <trd>&c_d;&c_z;&c_je;&c_s;&c_ja;&c_c;&c_mol; &c_z;&c_a;&c_p;&c_a;&c_v;&c_je;&c_d;&c_z;&c_ja;&c_w;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Dekalog</trd>,
    <trd>Zehn Gebote</trd>
  </trdgrp>
  <trd lng="es">dec&aacute;logo</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;calogue</trd>,
    <trd>dix commandements</trd>
  </trdgrp>
  <trd lng="hu">t&iacute;zparancsolat</trd>
  <trd lng="ru">&c_d;&c_je;&c_s;&c_ja;&c_t;&c_mol;
  &c_z;&c_a;&c_p;&c_o;&c_v;&c_je;&c_d;&c_je;&c_j;</trd>
  <trd lng="sk">desa&tcaron; <ind>prik&aacute;zan&iacute;</ind></trd>
</drv>
</art>
