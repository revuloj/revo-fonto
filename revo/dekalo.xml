<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dekalo.xml,v 1.23 2023/08/31 10:43:58 revo Exp $">
<kap>
  <rad>dekalog</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="dekalo.0o">
  <kap><tld/>o</kap>
  <snc mrk="dekalo.0o.BIB">
    <uzo tip="fak">BIB</uzo>
    <uzo tip="fak">KRI</uzo>
    <dif>
      La dek religiaj <ref tip="prt" cel="instru.0o.instruajxo">instruoj</ref>, 
      kiujn la Biblio prezentas kiel diktitajn de Javeo al <ref cel="mose.0o">Moseo</ref>,
      kaj kiuj fari&gcirc;is baza <ref tip="super" cel="legx.0o.JUR">le&gcirc;o</ref>
      de la hebrea popolo:
      <ekz>
        jen bonaj gvidlinioj, kiuj konstante akompanas nin dum nia 2000-jara
        civilizacio, ekde la <tld/>o al Moseo, &gcirc;is la deklaracio pri homaj rajtoj
        eldonita en 1948
        <fnt>
          <aut>Gilbert Ledon</aut>
            <vrk><url ref="&Monato;/2000/007018.php">Kulturo de milito</url></vrk>
            <bib>Monato</bib>
            <lok>2000/06, p. 5</lok>
          </fnt>;
      </ekz>
      <ekz>
        en nia kristana mondo temas pri la <tld/>o, aro da diaj ordonoj, kiuj devus
        direkti nian vivon
        <fnt>
          <bib>Monato</bib>
          <aut>Hradil Jozefo</aut>
          <vrk><url ref="&Monato;/2004/007952.html">&Ccirc;u la dekalogo ankora&ubreve; validas?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo><uzo tip="fak">ESP</uzo>
        la dek-kvina regulo de nia dek-ses-linia <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Petro Desmet'</aut>
          <vrk><url ref="&Monato;/2008/009292.php">Sidante en pudingo, mi alvokas miajn
          savantojn!</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_e;&c_k;&c_a;&c_l;&c_jo;&c_g;</trd>,
    <trd>&c_d;&c_z;&c_je;&c_s;&c_ja;&c_c;&c_mol;
    &c_z;&c_a;&c_p;&c_a;&c_v;&c_je;&c_d;&c_z;&c_ja;&c_w;</trd>
  </trdgrp>
  <trd lng="ca">dec&agrave;leg</trd>
  <trdgrp lng="cs">
    <trd>dekalog</trd>,
    <trd>desatero p&rcaron;ik&aacute;z&aacute;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Dekalog</trd>,
    <trd>Zehn Gebote</trd>
  </trdgrp>
  <trd lng="en">decalogue</trd>
  <trd lng="es">dec&aacute;logo</trd>
  <trd lng="fa">&a_d;&a_h;&#x200C;&a_f;&a_r;&a_m;&a_A;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;calogue</trd>,
    <trd>dix commandements</trd>
  </trdgrp>
  <trd lng="he">&#x5E2;&#x5E9;&#x5E8;&#x5EA; &#x5D4;&#x5D3;&#x5D1;&#x5E8;&#x5D5;&#x5EA;</trd>
  <trd lng="hu">t&iacute;zparancsolat</trd>
  <trdgrp lng="id">
    <trd>dekalog</trd>,
    <trd>Sepuluh Perintah Tuhan</trd>
  </trdgrp>
  <trd lng="ja">&#x5341;&#x6212;  [<ind>&#x3058;&#x3063;&#x304B;&#x3044;</ind>]</trd>
  <trdgrp lng="pl">
    <trd>Dekalog</trd>,
    <trd>Dziesi&eogonek;&cacute; Przykaza&nacute;<klr tip="amb">
    Bo&zdot;ych</klr></trd>
  </trdgrp>
  <trd lng="pt">dec&aacute;logo</trd>
  <trd lng="ru">&c_d;&c_je;&c_s;&c_ja;&c_t;&c_mol;
  &c_z;&c_a;&c_p;&c_o;&c_v;&c_je;&c_d;&c_je;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>desa&tcaron; <ind>prik&aacute;zan&iacute;</ind></trd>,
    <trd>desa&tcaron; prik&aacute;zan&iacute; <klr>(bo&zcaron;&iacute;ch)</klr></trd>
  </trdgrp>
  <trd lng="uk">&c_d;&c_je;&c_s;&#x44F;&c_t;&#x44C; &c_z;&c_a;&#x43F;&#x43E;&c_v;&c_ib;&c_d;&c_je;&c_j;</trd>
</drv>
</art>
