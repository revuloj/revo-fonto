        la <ind>E&ubreve;ropa <tld lit="K"/>umo de Homaj Rajtoj</ind> en
        Strasburgo
        <fnt>
	<aut>Ji&rcaron;i Patera</aut>
	<vrk><url ref="&Monato;/2001/007207.php">La mezepokaj judaj mortintoj fine
          trankvilaj</url></vrk>
	<bib>Monato</bib>
	<lok>2001/01, p. 14</lok>
        </fnt>;
        <trd lng="de">Europ&auml;ischer Gerichtshof f&uuml;r Menschenrechte</trd>
        <trd lng="nl">Europees <ind>Hof</ind> voor de Rechten van de Mens</trd>
      </ekz>
      <ekz>
        <ind>kasacia <tld/>umo</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2005/008533.php">Pri
          kato, rato, &scirc;tato kaj azeno</url></vrk>
          <lok>2005</lok>
        </fnt>;
        <trd lng="de">Kassationsgerichtshof</trd>
        <trd lng="nl">Hof van Cassatie</trd>
      </ekz>
      <ekz>
        <ind>administracia <tld/>umo</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2008/009472.php">Kaptukoj
          kaj kapdoloroj</url></vrk>
          <lok>2008</lok>
        </fnt>;
        <trd lng="de">Verwaltungsgerichtshof</trd>
      </ekz>
      <ekz>
        la hungara <ind>Konstitucia <tld lit="K"/>umo</ind> deklaris
        kontra&ubreve;konstituciaj la
        paragrafojn 199 kaj 200
        <fnt>
          <bib>Monato</bib>
          <aut>Istv&aacute;n Ertl</aut>
          <vrk><url ref="&Monato;/2003/007977.html">Gaji&gcirc;as
