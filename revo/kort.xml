<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kort</rad>/o
</kap>
<drv mrk="kort.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Sentegmenta spaco, &ccirc;irka&ubreve;ita de konstruoj, muroj a&ubreve; bariloj
      apud domo:
      <ekz>
        &scirc;i malsupreniris sur la korton<fnt>Z</fnt>;
      </ekz>
      <ekz>
        oni malfermis la pordegon kaj la veturilo eniris en la
        <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>en sia <tld/>o &ccirc;iu kok' estas forta<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        starus la morto jam en la <tld/>o<fnt>Z</fnt>
        <klr>(tre proksime)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">dv&ugrave;r</trd>
  <trd lng="de">Hof</trd>
  <trdgrp lng="en">
    <trd>court</trd>,
    <trd>enclosed area</trd>,
    <trd>quadrangle</trd>,
    <trd>yard</trd>
  </trdgrp>
  <trd lng="fr">cour</trd>
  <trd lng="ru">&c_d;&c_v;&c_o;&c_r;</trd>
  <trd lng="sv">g&aring;rdsplan</trd>
</drv>

<drv mrk="kort.0ego">
  <kap>
    <tld/>ego
  </kap>
  <snc>
    <dif>
      &Ccirc;irka&ubreve;antaro de regnestro a&ubreve; princo:
      <ekz>
        <tld/>ega konsilanto.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Hofstaat</trd>
  <trdgrp lng="en">
    <trd>court</trd>,
    <trd>king's retinue</trd>,
    <trd>royal household</trd>
  </trdgrp>
  <trd lng="fr">cour</trd>
  <trd lng="ru">&c_d;&c_v;&c_o;&c_r; <klr>(&c_k;&c_o;&c_r;&c_o;&c_l;&c_je;&c_v;&c_s;&c_k;&c_i;&c_j;, &c_c;&c_a;&c_r;&c_s;&c_k;&c_i;&c_j; &c_i; &c_t;.&c_p;.)</klr></trd>
  <trd lng="sv">hov <klr tip="ind">(hos furste, kung, etc.)</klr></trd>
</drv>

<drv mrk="kort.0egano">
  <kap>
    <tld/>egano
  </kap>
  <snc>
    <dif>
      Persono apartenanta al la <tld/>ego.
    </dif>
  </snc>
  <trd lng="de">H&ouml;fling</trd>
  <trd lng="en">courtier</trd>
  <trd lng="fr">courtisan</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_d;&c_v;&c_o;&c_r;&c_n;&c_y;&c_j; <klr>(&c_s;&c_u;&c_shch;.)</klr></trd>
</drv>

<drv mrk="kort.0umo">
  <kap>
    <tld/>umo
  </kap>
  <snc>
    <dif>
      Supera ju&gcirc;istaro.
    </dif>
  </snc>
  <trd lng="de">Gerichtshof</trd>
  <trd lng="en">court of law </trd>
  <trd lng="fr">cour de justice</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_r;&c_h;&c_o;&c_v;&c_n;&c_y;&c_j; &c_s;&c_u;&c_d;</trd>,
    <trd>&c_s;&c_u;&c_d; &c_v;&c_y;&c_s;&c_sh;&c_je;&c_j; &c_i;&c_n;&c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="kort.birdo0o">
  <kap>
    birdo<tld/>o
  </kap>
  <snc>
    <dif>
      Speciala <tld/>o rezervita al la hejmaj bestoj.
    </dif>
  </snc>
  <trd lng="en">barnyard</trd>
  <trd lng="fr">basse-cour</trd>
  <trd lng="ru">&c_p;&c_t;&c_i;&c_ch;&c_i;&c_j; &c_d;&c_v;&c_o;&c_r;</trd>
</drv>

<drv mrk="kort.0obirdo">
  <kap>
    <tld/>obirdo
  </kap>
  <snc>
    <dif>
      Birdo bredita en birdo<tld/>o por siaj viando a&ubreve; ovoj:
      <ekz>
        anaso, koko kaj meleagro estas <tld/>obirdoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Gefl&uuml;gel</trd>
  <trdgrp lng="en">
    <trd>farm bird</trd>,
    <trd>domestic fowl</trd>
  </trdgrp>
  <trd lng="fr">volaille</trd>
  <trd lng="ru">&c_d;&c_o;&c_m;&c_a;&c_sh;&c_n;&c_ja;&c_ja; &c_p;&c_t;&c_i;&c_c;&c_a;</trd>
</drv>
</art>
