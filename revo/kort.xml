<drv mrk="kort.0ego">
  <kap><ofc>*</ofc><tld/>ego</kap>
  <snc mrk="kort.0ego.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      &Ccirc;irka&ubreve;antaro de regnestro a&ubreve; princo:
      <ekz>
        la <tld/>ego de la re&gcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 2:49</lok>
        </fnt>;
      </ekz>
      <ekz>
        la imperiestra <tld/>ego de Rusujo
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>ego de la papo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        vespere ni havos balon de <tld/>ego
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i estis tiel ravante bela, ke la <tld/>ego <klr>[&#8230;]</klr>
        profunde klini&gcirc;is
        anta&ubreve; &scirc;i
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la re&gcirc;ino kaj la tuta <tld/>ego atendis
        <fnt>
          <bib>Fab1</bib>
          <lok>Fluganta kofro</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la <tld/>ego li ellernis paciencon kaj malsincerecon
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 13a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>ega kuiristino
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>egestro
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_v;&c_o;&c_r; <klr tip="amb">(&c_n;&c_a;&c_p;&c_r;.,
&c_k;&c_a;&c_r;&c_a;&c_l;&c_je;&c_w;&c_s;&c_k;&c_ib;)</klr></trd>
  <trdgrp lng="cs">
    <trd>panovnick&yacute; dv&uring;r</trd>,
    <trd>&scaron;lechtick&yacute; dv&uring;r</trd>
  </trdgrp>
  <trd lng="de">Hofstaat</trd>
  <trdgrp lng="en">
    <trd>court</trd>,
    <trd>king's retinue</trd>,
    <trd>royal household</trd>
  </trdgrp>
  <trd lng="es">corte (de un soberano)</trd>
  <trdgrp lng="fa">
    <trd>&a_d;&a_r;&a_b;&a_A;&a_r;</trd>,
