<drv mrk="kort.0egano">
  <kap><tld/>egano</kap>
  <snc mrk="kort.0egano.persono">
    <dif>
      Persono apartenanta al la <tld/>ego:
      <ekz>
        <tld/>egano de Faraono, estro de la korpogardistoj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 39:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        du <tld/>eganoj de la re&gcirc;o el la sojlogardistoj
        <fnt>
          <bib>MT</bib>
          <lok>Ester 2:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>egano-konsilanto
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        post kiam rusaj caroj donacis terpecojn al siaj <tld/>eganoj,
        komenci&gcirc;is
        disfloro de la regiono
        <fnt>
          <bib>Monato</bib>
          <aut>Dmitrij Cibulevskij</aut>
          <vrk><url ref="&Monato;/2009/009763.php">Foroso</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        feste vestitaj <tld/>eganoj atendis la invititojn anta&ubreve; la
        impona pordego de la palaco
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 10a, Babel</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_d;&c_v;&c_o;&c_r;&c_n;&c_y;</trd>
  <trd lng="cs">dvo&rcaron;an</trd>
  <trd lng="de">H&ouml;fling</trd>
  <trd lng="en">courtier</trd>
  <trd lng="es">cortesano</trd>
  <trd lng="fa">&a_d;&a_r;&a_b;&a_A;&a_r;&f_y;</trd>
  <trd lng="fr">courtisan</trd>
  <trdgrp lng="hu">
    <trd>udvaronc</trd>,
    <trd>udvari ember</trd>
  </trdgrp>
  <trd lng="id">abdi dalem</trd>
  <trd lng="nl">hoveling</trd>
  <trdgrp lng="pl">
    <trd>dworzanin</trd>,
    <trd>dw&oacute;rka</trd>
  </trdgrp>
  <trd lng="ro">curtean</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_d;&c_v;&c_o;&c_r;&c_n;&c_y;&c_j; 
  <klr>(&c_s;&c_u;&c_shch;.)</klr></trd>
  <trd lng="sk">dvoran</trd>
</drv>

<drv mrk="kort.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="kort.0isto.pordisto">
    <dif>
      <ref tip="dif" cel="pord.0isto">Pordisto</ref>:
      <ekz>
        je l' deka horo vespere la <tld/>isto fermas la pordegon de l'
        domo
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <klr>[&#8230;]</klr> kuris malsupren al la lo&gcirc;ejo de la
        <tld/>isto
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        a&ubreve;deblas bruo de <tld/>aj katoj kaj hundoj kaj de <tld/>isto,
        kiu same frumatene
        komencas sian &ccirc;iutagan laboron
        <fnt>
          <bib>Monato</bib>
          <aut>Dmitrij Cibulevskij</aut>
          <vrk><url ref="&Monato;/2011/010371.php">Unu tago
          de iu persono</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Hauswart</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_d;&c_z;&c_mol;&c_v;&c_je;&c_r;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_a;&c_d;&c_z;&c_mol;&c_v;&c_je;&c_r;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_m;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_b;&c_r;&c_a;&c_m;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="cs">domovn&iacute;k</trd>
  <trdgrp lng="en">
    <trd>door attendant</trd>,
    <trd>doorman</trd>,
    <trd>gatekeeper</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_d;&a_r;&a_b;&a_A;&a_n;</trd>,
    <trd>&a_s;&a_r;&a_A;&f_y;&a_d;&a_A;&a_r;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>concierge</trd>,
    <trd>portier</trd>
  </trdgrp>
  <trd lng="id">pen<ind>jaga</ind> pintu</trd>
  <trd lng="nl">portier</trd>
  <trdgrp lng="pl">
    <trd>portier</trd>,
    <trd>str&oacute;&zdot;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>portar</trd>,
    <trd>str&abreve;jer</trd>
  </trdgrp>
  <trd lng="sk">domovn&iacute;k</trd>
