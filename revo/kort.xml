    <trd>king's retinue</trd>,
    <trd>royal household</trd>
  </trdgrp>
  <trd lng="es">corte (de un soberano)</trd>
  <trdgrp lng="fa">
    <trd>&a_d;&a_r;&a_b;&a_A;&a_r;</trd>,
