  <trd lng="ro">curte domneasc&abreve;</trd>
  <trd lng="ru">&c_d;&c_v;&c_o;&c_r;<klr> (&c_k;&c_o;&c_r;&c_o;&c_l;&c_je;&c_v;&c_s;&c_k;&c_i;&c_j;,
