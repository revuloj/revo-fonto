<drv mrk="kort.0umo">
  <kap><tld/>umo</kap>
  <snc mrk="kort.0umo.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Nomo de diversaj ju&gcirc;instancoj, precipe de supera nivelo:
      <ekz>
        <nom>Haughey</nom> estas alvokita aperi en marto 2000 anta&ubreve;
        ju&gcirc;isto de supera <tld/>umo
        <fnt>
          <bib>Monato</bib>
          <aut>PGubbins</aut>
          <vrk><url ref="&Steloj;monato2/006914.html">Irlando:
          Lando de malsanktuloj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li estis arestita kaj devis aperi en la urba <tld/>umo anta&ubreve;
        ju&gcirc;isto
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2009/009680.php">Proksimaj kaj
          malproksimaj najbaroj</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        ju&gcirc;isto <klr>[&#8230;]</klr> eksi&gcirc;is kiel membro de la plej alta
        irlanda <tld/>umo por
        eviti akuzojn, ke li kontra&ubreve;le&gcirc;e agis
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="&Steloj;monato2/007134.html">Malbona
          investo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <ind>E&ubreve;ropa <tld lit="K"/>umo de Homaj Rajtoj</ind> en
        Strasburgo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/007207.html">La
          mezepokaj judaj mortintoj fine trankvilaj</url></vrk>
        </fnt>;
        <trd lng="de">Europ&auml;ischer Gerichtshof f&uuml;r Menscherechte</trd>
      </ekz>
      <ekz>
        <ind>kasacia <tld/>umo</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2005/008533.php">Pri
          kato, rato, &scirc;tato kaj azeno</url></vrk>
          <lok>2005</lok>
        </fnt>;
        <trd lng="de">Kassationsgerichtshof</trd>
      </ekz>
      <ekz>
        <ind>administracia <tld/>umo</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2008/009472.php">Kaptukoj
          kaj kapdoloroj</url></vrk>
          <lok>2008</lok>
        </fnt>;
        <trd lng="de">Verwaltungsgerichtshof</trd>
      </ekz>
      <ekz>
        la hungara <ind>Konstitucia <tld lit="K"/>umo</ind> deklaris
        kontra&ubreve;konstituciaj la
        paragrafojn 199 kaj 200
        <fnt>
          <bib>Monato</bib>
          <aut>Istv&aacute;n Ertl</aut>
          <vrk><url ref="&Monato;/2003/007977.html">Gaji&gcirc;as
          gejoj</url></vrk>
