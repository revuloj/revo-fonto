<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kort.xml,v 1.36 2020/02/05 22:10:18 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kort</rad>/o
</kap>
<drv mrk="kort.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kort.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Sentegmenta spaco, &ccirc;irka&ubreve;ita de konstruoj, muroj
      a&ubreve; bariloj apud domo:
      <ekz>
        oni malfermis la pordegon, kaj la kale&scirc;o enveturis en la
        <tld/>on
        <fnt><bib>F</bib>, <lok>&FE; 38</lok></fnt>;
      </ekz>
      <ekz>
        granda &ccirc;irka&ubreve;a korto <klr>[kaj]</klr> interna korto de la domo
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 7:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de malliberejo
        <fnt>
          <bib>MT</bib>
          <lok>Ne&hcirc;emja 3:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <klr>[&#8230;]</klr> malsupreniris sur la <tld/>on al la puto
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        en sia <tld/>o &ccirc;iu kok' estas forta
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        staras la morto jam en la <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>
        <klr>(tre proksime)</klr>;
      </ekz>
      <ekz>
        sub la tegmento de policeja <tld/>o ni nodligis niajn hamakojn
        <fnt>
          <bib>Monato</bib>
          <aut>Werner Schad</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006868.html">Venezuelo: Inter maro kaj arbarego</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_v;&c_o;&c_r;</trd>
  <trd lng="bg">&c_d;&c_v;&c_o;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>dv&uring;r</trd>,
    <trd>dvorec</trd>,
    <trd>dv&uring;r</trd>,
    <trd>n&aacute;dvo&rcaron;&iacute;</trd>
  </trdgrp>
  <trd lng="de">Hof</trd>
  <trdgrp lng="en">
    <trd>court</trd>,
    <trd>courtyard</trd>,
    <trd>enclosed area</trd>,
    <trd>patio</trd>,
    <trd>quadrangle</trd>,
    <trd>yard</trd>
  </trdgrp>
  <trd lng="es">patio (interno)</trd>
  <trdgrp lng="fa">
    <trd>&a_H;&f_y;&a_A;&a_T;</trd>,
    <trd>&a_S;&a_H;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cour<klr> (de maison)</klr></trd>,
    <trd>parvis</trd>
  </trdgrp>
  <trd lng="hu">udvar</trd>
  <trdgrp lng="id">
    <trd><ind>halaman</ind> (rumah)</trd>,
    <trd>[<ind>karang</ind>] pekarangan</trd>,
    <trd><ind>lapang</ind>an</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>hof</trd>,
    <trd>binnenplaats</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>podw&oacute;rko</trd>,
    <trd>podw&oacute;rze</trd>,
    <trd>dziedziniec</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>p&aacute;tio</trd>,
    <trd>quintal</trd>,
    <trd>terreiro</trd>,
    <trd>eirado</trd>
  </trdgrp>
  <trd lng="ro">curte</trd>
  <trd lng="ru">&c_d;&c_v;&c_o;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>dvor</trd>,
    <trd>n&aacute;dvorie</trd>
  </trdgrp>
  <trd lng="sv">g&aring;rdsplan</trd>
  <trd lng="zh">&#38498;&#23376; [<ind>yu&agrave;nzi</ind>]</trd>
</drv>

<drv mrk="kort.0ego">
  <kap><ofc>*</ofc><tld/>ego</kap>
  <snc mrk="kort.0ego.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      &Ccirc;irka&ubreve;antaro de regnestro a&ubreve; princo:
      <ekz>
        la <tld/>ego de la re&gcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 2:49</lok>
        </fnt>;
      </ekz>
      <ekz>
        la imperiestra <tld/>ego de Rusujo
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>ego de la papo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        vespere ni havos balon de <tld/>ego
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i estis tiel ravante bela, ke la <tld/>ego <klr>[&#8230;]</klr> profunde klini&gcirc;is
        anta&ubreve; &scirc;i
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la re&gcirc;ino kaj la tuta <tld/>ego atendis
        <fnt>
          <bib>Fab1</bib>
          <lok>Fluganta kofro</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la <tld/>ego li ellernis paciencon kaj malsincerecon
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 13a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>ega kuiristino
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>egestro
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_v;&c_o;&c_r; <klr tip="amb">(&c_n;&c_a;&c_p;&c_r;.,
&c_k;&c_a;&c_r;&c_a;&c_l;&c_je;&c_w;&c_s;&c_k;&c_ib;)</klr></trd>
  <trdgrp lng="cs">
    <trd>panovnick&yacute; dv&uring;r</trd>,
    <trd>&scaron;lechtick&yacute; dv&uring;r</trd>
  </trdgrp>
  <trd lng="de">Hofstaat</trd>
  <trdgrp lng="en">
    <trd>court</trd>,
    <trd>king's retinue</trd>,
    <trd>royal household</trd>
  </trdgrp>
  <trd lng="es">corte (de un soberano)</trd>
  <trdgrp lng="fa">
    <trd>&a_d;&a_r;&a_b;&a_A;&a_r;</trd>,
    <trd>&a_d;&a_r;&a_b;&a_A;&a_r;&f_y;&a_A;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">cour<klr> (d'un souverain)</klr></trd>
  <trdgrp lng="hu">
    <trd>udvar <klr tip="ind">(pl. kir&aacute;lyi)</klr></trd>,
    <trd>udvartart&aacute;s</trd>
  </trdgrp>
  <trd lng="id"><ind>keluarga</ind> kerajaan</trd>
  <trd lng="nl">hof<klr tip="ind"> (koninklijk)</klr></trd>
  <trd lng="pl">dw&oacute;r<klr> (kr&oacute;lewski, ksi&aogonek;&zdot;&eogonek;cy)</klr></trd>
  <trd lng="ro">curte domneasc&abreve;</trd>
  <trd lng="ru">&c_d;&c_v;&c_o;&c_r;<klr> (&c_k;&c_o;&c_r;&c_o;&c_l;&c_je;&c_v;&c_s;&c_k;&c_i;&c_j;, &c_c;&c_a;&c_r;&c_s;&c_k;&c_i;&c_j; &c_i; &c_t;.&c_p;.)</klr></trd>
  <trd lng="sk">panovn&iacute;cky dvor</trd>
  <trd lng="sv">hov <klr tip="ind">(hos furste, kung, etc.)</klr></trd>
</drv>

<drv mrk="kort.0egano">
  <kap><tld/>egano</kap>
  <snc mrk="kort.0egano.persono">
    <dif>
      Persono apartenanta al la <tld/>ego:
      <ekz>
        <tld/>egano de Faraono, estro de la korpogardistoj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 39:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        du <tld/>eganoj de la re&gcirc;o el la sojlogardistoj
        <fnt>
          <bib>MT</bib>
          <lok>Ester 2:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>egano-konsilanto
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        post kiam rusaj caroj donacis terpecojn al siaj <tld/>eganoj, komenci&gcirc;is
        disfloro de la regiono
        <fnt>
          <bib>Monato</bib>
          <aut>Dmitrij Cibulevskij</aut>
          <vrk><url ref="https://www.monato.be/2009/009763.php">Foroso</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        feste vestitaj <tld/>eganoj atendis la invititojn anta&ubreve; la
        impona pordego de la palaco
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 10a, Babel</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_d;&c_v;&c_o;&c_r;&c_n;&c_y;</trd>
  <trd lng="cs">dvo&rcaron;an</trd>
  <trd lng="de">H&ouml;fling</trd>
  <trd lng="en">courtier</trd>
  <trd lng="es">cortesano</trd>
  <trd lng="fa">&a_d;&a_r;&a_b;&a_A;&a_r;&f_y;</trd>
  <trd lng="fr">courtisan</trd>
  <trdgrp lng="hu">
    <trd>udvaronc</trd>,
    <trd>udvari ember</trd>
  </trdgrp>
  <trd lng="id">abdi dalem</trd>
  <trd lng="nl">hoveling</trd>
  <trdgrp lng="pl">
    <trd>dworzanin</trd>,
    <trd>dw&oacute;rka</trd>
  </trdgrp>
  <trd lng="ro">curtean</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_d;&c_v;&c_o;&c_r;&c_n;&c_y;&c_j;  <klr>(&c_s;&c_u;&c_shch;.)</klr></trd>
  <trd lng="sk">dvoran</trd>
</drv>

<drv mrk="kort.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="kort.0isto.pordisto">
    <dif>
      <ref tip="dif" cel="pord.0isto">Pordisto</ref>:
      <ekz>
        je l' deka horo vespere la <tld/>isto fermas la pordegon de l' domo
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <klr>[&#8230;]</klr> kuris malsupren al la lo&gcirc;ejo de la
        <tld/>isto
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        a&ubreve;deblas bruo de <tld/>aj katoj kaj hundoj kaj de <tld/>isto, kiu same frumatene
        komencas sian &ccirc;iutagan laboron
        <fnt>
          <bib>Monato</bib>
          <aut>Dmitrij Cibulevskij</aut>
          <vrk><url ref="https://www.monato.be/2011/010371.php">Unu tago de iu persono</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Hauswart</trd>
  </snc>
  <trd lng="cs">domovn&iacute;k</trd>
  <trdgrp lng="fa">
    <trd>&a_d;&a_r;&a_b;&a_A;&a_n;</trd>,
    <trd>&a_s;&a_r;&a_A;&f_y;&a_d;&a_A;&a_r;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>concierge</trd>,
    <trd>portier</trd>
  </trdgrp>
  <trd lng="id">pen<ind>jaga</ind> pintu</trd>
  <trd lng="nl">portier</trd>
  <trdgrp lng="pl">
    <trd>portier</trd>,
    <trd>str&oacute;&zdot;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>portar</trd>,
    <trd>str&abreve;jer</trd>
  </trdgrp>
  <trd lng="sk">domovn&iacute;k</trd>
</drv>

<drv mrk="kort.0umo">
  <kap><tld/>umo</kap>
  <snc mrk="kort.0umo.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Nomo de diversaj ju&gcirc;instancoj, precipe de supera nivelo:
      <ekz>
        <nom>Haughey</nom> estas alvokita aperi en marto 2000 anta&ubreve; ju&gcirc;isto de supera <tld/>umo
        <fnt>
          <bib>Monato</bib>
          <aut>PGubbins</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006914.html">Irlando: Lando de malsanktuloj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li estis arestita kaj devis aperi en la urba <tld/>umo anta&ubreve; ju&gcirc;isto
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="https://www.monato.be/2009/009680.php">Proksimaj kaj malproksimaj najbaroj</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        ju&gcirc;isto <klr>[&#8230;]</klr> eksi&gcirc;is kiel membro de la plej alta irlanda <tld/>umo por
        eviti akuzojn, ke li kontra&ubreve;le&gcirc;e agis
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007134.html">Malbona investo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <ind>E&ubreve;ropa <tld lit="K"/>umo de Homaj Rajtoj</ind> en Strasburgo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007207.html">La mezepokaj judaj mortintoj fine trankvilaj</url></vrk>
        </fnt>;
        <trd lng="de">Europ&auml;ischer Gerichtshof f&uuml;r Menscherechte</trd>
      </ekz>
      <ekz>
        <ind>kasacia <tld/>umo</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="https://www.monato.be/2005/008533.php">Pri kato, rato, &scirc;tato kaj azeno</url></vrk>
          <lok>2005</lok>
        </fnt>;
        <trd lng="de">Kassationsgerichtshof</trd>
      </ekz>
      <ekz>
        <ind>administracia <tld/>umo</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="https://www.monato.be/2008/009472.php">Kaptukoj kaj kapdoloroj</url></vrk>
          <lok>2008</lok>
        </fnt>;
        <trd lng="de">Verwaltungsgerichtshof</trd>
      </ekz>
      <ekz>
        la hungara <ind>Konstitucia <tld lit="K"/>umo</ind> deklaris kontra&ubreve;konstituciaj la
        paragrafojn 199 kaj 200
        <fnt>
          <bib>Monato</bib>
          <aut>Istv&aacute;n Ertl</aut>
          <vrk><url ref="https://www.monato.be/2003/007977.html">Gaji&gcirc;as gejoj</url></vrk>
          <lok>2003</lok>
        </fnt>;
        <trd lng="de">Verfassungsgerichtshof</trd>
      </ekz>
      <ekz>
        komenci&gcirc;is en Hago &ccirc;e la <ind>Internacia <tld lit="K"/>umo</ind> proceso de Irano kontra&ubreve;
        Usono
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="https://www.monato.be/2003/008061.html">Usono inter Irano kaj Irako</url></vrk>
          <lok>2003</lok>
        </fnt>.
        <trd lng="de">Internationaler Gerichtshof</trd>
      </ekz>
    </dif>
    <ref tip="vid" cel="tribun1.0o.jugxistaro">tribunalo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_j;&c_v;&c_y;&c_sh;&c_e;&c_j;&c_sh;&c_y;&c_s;&c_u;&c_d;</trd>,
    <trd>&c_s;&c_u;&c_d; &c_n;&c_a;&c_j;&c_v;&c_y;&c_sh;&c_e;&c_j;&c_sh;&c_a;&c_j; &c_ib;&c_n;&c_s;&c_t;&c_a;&c_n;&c_c;&c_y;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dv&uring;r <klr>(soudn&iacute;, apela&ccaron;n&iacute;)</klr></trd>,
    <trd>soud</trd>
  </trdgrp>
  <trd lng="de">Gerichtshof</trd>
  <trd lng="en">court of law </trd>
  <trdgrp lng="es">
    <trd>corte (jur&iacute;dica)</trd>,
    <trd>tribunal</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_d;&f_y;&a_w;&a_A;&a_n;</trd>,
    <trd>&a_d;&a_A;&a_d;&f_g;&a_A;&a_h;</trd>,
    <trd>&a_m;&a_H;&f_k;&a_m;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">cour<klr> (de justice)</klr></trd>
  <trd lng="hu">b&iacute;r&oacute;s&aacute;g
    <klr tip="ind">(fels&odblac; fok&uacute;)</klr></trd>
  <trd lng="id">[<ind>adil</ind>] pengadilan</trd>
  <trdgrp lng="nl">
    <trd>hof<klr tip="ind"> (gerechtshof)</klr></trd>,
    <trd>gerechtshof</trd>
  </trdgrp>
  <trd lng="pl">s&aogonek;d</trd>     
  <trd lng="ro">tribunal</trd>  
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_r;&c_h;&c_o;&c_v;&c_n;&c_y;&c_j; &c_s;&c_u;&c_d;</trd>,
    <trd>&c_s;&c_u;&c_d; &c_v;&c_y;&c_s;&c_sh;&c_je;&c_j; &c_i;&c_n;&c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dvor <klr>(s&uacute;dny, apela&ccaron;n&yacute;)</klr></trd>,
    <trd>s&uacute;d</trd>
  </trdgrp>
</drv>

<drv mrk="kort.antaux0o">
  <kap>anta&ubreve;<tld/>o</kap>
  <snc mrk="kort.antaux0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <tld lit="K"/>o, anta&ubreve;anta la &ccirc;efan <tld/>on:
      <ekz>
        li aran&gcirc;is la <tld/>on de la pastroj kaj la grandan
        anta&ubreve;<tld/>on kaj pordojn por la anta&ubreve;<tld/>o
        <fnt><bib>MT</bib>, <lok>&Kr2; 4:9</lok></fnt>;
      </ekz>
      <ekz>
        la origina koncepto de la elektistoprinco planis Cvingeron kiel anta&ubreve;<tld/>o de
        nova kastelo, kiu okupu la spacon &gcirc;is la Elbo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=450812"
              >Zwinger (Dresdeno)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Vorhof</trd>
  </snc>
  <trd lng="fa">&a_H;&f_y;&a_A;&a_T; &a_b;&f_y;&a_r;&a_w;&a_n;&f_y;</trd>
  <trd lng="fr">avant-cour</trd>
  <trd lng="hu">el&odblac;udvar</trd>
  <trd lng="nl">voorhof</trd>
  <trd lng="pl">przedsionek</trd>
  <trdgrp lng="ro">
    <trd>prag</trd>,
    <trd>vestibul</trd>
  </trdgrp>
  <adm>
    Mi transprenis la difinon de PIV, sed mi ne certas, cxu
    la interpreto estas gxusta kadre de la Biblia ekzemplo
    (templo de Salomono).
    <aut>MB</aut>
  </adm>
</drv>

<drv mrk="kort.birdo0o">
  <kap>birdo<tld/>o</kap>
  <snc mrk="kort.birdo0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
