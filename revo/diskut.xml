<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: diskut.xml,v 1.22 2021/05/29 20:09:53 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>diskut</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="diskut.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="diskut.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="parol.0i.diri">Paroli</ref>,
      interargumentante pri malsamaj opinioj:
      <ekz>
        <tld/>i kun iu pri politiko, religio, pri la situacio;
      </ekz>
      <ekz>
        oni longe <tld/>is pri tiu propono;
      </ekz>
      <ekz>
        Ina ankora&ubreve; ne konis &scirc;in sufi&ccirc;e bone
        por <tld/>i kun &scirc;i intima&jcirc;ojn
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="debat.0i">debati</ref>,
      <ref cel="disput.0i">disputi</ref>,
      <ref cel="polemi.0i">polemiki</ref>
    </refgrp>
  </snc>
  <trd lng="af">bespreek</trd>
  <trdgrp lng="be">
    <trd>&c_d;&c_y;&c_s;&c_k;&c_u;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_m;&c_ja;&c_r;&c_k;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&#xF66;&#xF90;&#xF51;&#xF0B;&#xF46;&#xF0B;&#xF56;&#xF64;&#xF51;</trd>
  <trdgrp lng="cs">
    <trd>debatovat</trd>,
    <trd>diskutovat</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>diskutieren</trd>,
     <trd>er&ouml;rtern</trd>
  </trdgrp>
  <trd lng="en">discuss</trd>
  <trd lng="es">discutir</trd>
  <trd lng="fr">discuter<klr tip="ind"> (intr.)</klr></trd>
  <trd lng="ne">&#x91B;&#x932;&#x92B;&#x932;</trd>
  <trdgrp lng="pl">
    <trd>dyskutowa&cacute;</trd>,
    <trd>rozprawia&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>discutir</trd>,
    <trd>argumentar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_s;&c_u;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_i;&c_s;&c_k;&c_u;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">diskutova&tcaron;</trd>
  <trd lng="sq">diskutuar</trd>
  <trd lng="sv">diskutera</trd>
</drv>

<drv mrk="diskut.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="diskut.pri0i.KOMUNE">
    <dif>
      Ekzameni opinion, argumentante pri &gcirc;ia praveco a&ubreve;
      malpraveco:
      <ekz>
        pri<tld/>i proponon, la politikon de la partio.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kontes.0i">kontesti</ref>,
      <ref cel="refut.0i">refuti</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_m;&c_ja;&c_r;&c_k;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">prodiskutovat</trd>
  <trd lng="fr">discuter<klr tip="ind"> (tr.)</klr></trd>
  <trd lng="he">&#x5DC;&#x5B8;&#x5D3;&#x5D5;&#x5BC;&#x5DF; &#x5D1;</trd>
  <trd lng="pl">przedyskutowa&cacute;</trd>
  <trd lng="ru">&c_o;&c_b;&c_s;&c_u;&c_d;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">prediskutova&tcaron;</trd>
</drv>

<drv mrk="diskut.0ebla">
  <kap><tld/>ebla, <var><kap>kontra&ubreve;<tld/>ebla</kap></var></kap>
  <snc mrk="diskut.0ebla.KOMUNE">
    <dif>
      Ne povanta esti akceptata sen <tld/>o.
    </dif>
    <ref tip="vid" cel="dub.0a">duba</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_y;&c_s;&c_k;&c_u;&c_s;&c_ib;&c_j;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_p;&c_r;&c_e;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>diskutabiln&iacute;</trd>,
    <trd>diskutovateln&yacute;</trd>,
    <trd>pochybn&yacute;</trd>,
    <trd>sporn&yacute;</trd>
  </trdgrp>
  <trd lng="de">diskutabel</trd>
  <trd lng="es">discutible</trd>
  <trdgrp lng="fr">
    <trd>contestable</trd>,
    <trd>controvers&eacute;</trd>,
    <trd>discutable</trd>
  </trdgrp>
  <trd lng="he">&#x5E9;&#x5C1;&#x5B8;&#x5E0;&#x5D5;&#x5BC;&#x5D9; &#x5D1;&#x5BC;&#x5B0;&#x5DE;&#x5B7;&#x5D7;&#x5B2;&#x5DC;&#x5D5;&#x5B9;&#x5E7;&#x5B6;&#x5EA;</trd>
  <trdgrp lng="pl">
    <trd>dyskusyjny</trd>,
    <trd>sporny</trd>,
    <trd>kontrowersyjny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_p;&c_o;&c_r;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pochybn&yacute;</trd>,
    <trd>sporn&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="diskut.0inda">
  <kap><tld/>inda</kap>
  <snc mrk="diskut.0inda.KOMUNE">
    <dif>
      Estanta inda je <tld/>a klarigo. 
    </dif>
    <refgrp tip="vid">
      <ref cel="serioz.0a">serioza</ref>
    </refgrp>
    </snc>
  <trd lng="be">&c_s;&c_p;&c_r;&c_e;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>vhodn&yacute; k diskuzi</trd>,
    <trd>vhodn&yacute; k prodiskutov&aacute;n&iacute;</trd>,
    <trd>zasluhuj&iacute;c&iacute; diskusi</trd>
  </trdgrp>
  <trd lng="de">diskussionsw&uuml;rdig</trd>
  <trdgrp lng="fr">
    <trd>&agrave; <ind>approfondir</ind></trd>,
    <trd>&agrave; <ind>discuter</ind></trd></trdgrp>
  <trd lng="he">&#x5E8;&#x5D0;&#x5D5;&#x5D9; &#x5DC;&#x5D3;&#x5D9;&#x5D5;&#x5DF;</trd>
  <trd lng="ru">&c_s;&c_p;&c_o;&c_r;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">vhodn&yacute; na diskusiu</trd>
</drv>
</art>
