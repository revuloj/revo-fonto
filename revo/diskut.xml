<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>diskut</rad>/i
</kap>

<drv mrk="diskut.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Interargumenti pri malsamaj opinioj;
      ekzameni opinion, argumentante pri &gcirc;ia praveco a&ubreve;
      malpraveco:
      <ekz><tld/>i kun iu pri politiko, religio, pri la situacio;</ekz>
      <ekz>oni longe <tld/>is pri tiu propono.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="disput.0i">disputi</ref>,
      <ref cel="polemi.0i">polemiki</ref>,
      <ref cel="debat.0i">debati</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_y;&c_s;&c_k;&c_u;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_m;&c_ja;&c_r;&c_k;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>diskutieren</trd>,
     <trd>er&ouml;rtern</trd>
  </trdgrp>
  <trd lng="en">discuss</trd>
  <trd lng="es">discutir</trd>
  <trd lng="fr">discuter<klr tip="ind"> (intr.)</klr></trd>
  <trdgrp lng="pl">
    <trd>dyskutowa&cacute;</trd>,
    <trd>rozprawia&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>discutir</trd>,
    <trd>argumentar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_s;&c_u;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_i;&c_s;&c_k;&c_u;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">diskutera</trd>
</drv>

<drv mrk="diskut.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Samsence: <ekz><tld/>i proponon, la politikon de la partio.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="refut.0i">refuti</ref>,
      <ref cel="kontes.0i">kontesti</ref>.
    </refgrp>
  </snc>
  <trd
  lng="be">&c_a;&c_b;&c_m;&c_ja;&c_r;&c_k;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="fr">discuter<klr tip="ind"> (tr.)</klr></trd>
  <trd lng="pl">przedyskutowa&cacute;</trd>
  <trd lng="ru">&c_o;&c_b;&c_s;&c_u;&c_d;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="diskut.0ebla">
  <kap>(kontra&ubreve;)<tld/>ebla</kap>
  <snc>
    <dif>
      Ne povanta esti akceptata sen <tld/>o.
    </dif>
    <ref tip="vid" cel="dub.0a">duba</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_y;&c_s;&c_k;&c_u;&c_s;&c_ib;&c_j;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_p;&c_r;&c_e;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">diskutabel</trd>
  <trd lng="es">discutible</trd>
  <trdgrp lng="fr">
    <trd>contestable</trd>,
    <trd>controvers&eacute;</trd>,
    <trd>discutable</trd>
  </trdgrp> 
  <trdgrp lng="pl">
    <trd>dyskusyjny</trd>,
    <trd>sporny</trd>,
    <trd>kontrowersyjny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_p;&c_o;&c_r;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="diskut.0inda">
  <kap><tld/>inda</kap>
  <snc>
    <dif>
      Estanta inda je <tld/>a klarigo. 
    </dif>
    <refgrp tip="vid">
      <ref cel="serioz.0a">serioza</ref>.
    </refgrp>
    </snc>
  <trd lng="be">&c_s;&c_p;&c_r;&c_e;&c_ch;&c_n;&c_y;</trd>
  <trd lng="de">diskussionsw&uuml;rdig</trd>
  <trdgrp lng="fr">
    <trd>&agrave; <ind>approfondir</ind></trd>,
    <trd>&agrave; <ind>discuter</ind></trd></trdgrp> 
  <trd lng="ru">&c_s;&c_p;&c_o;&c_r;&c_n;&c_y;&c_j;</trd>
  </drv>
</art>
