<drv mrk="diskut.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Ago <tld/>i; inter&scirc;an&gcirc;i argumentojn pri malsamaj opinioj:
      <ekz>
        fanfaronema, nenion sciante, sed malsana je demandoj kaj privortaj <tld/>oj, de
        kiuj naski&gcirc;as envio, malpaco
        <fnt>
          <bib>NT</bib>
          <lok>I. Timoteo 6:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        se vi a&ubreve;dus niajn <tld/>ojn, se vi vidus la mienojn de niaj konsilantoj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>!
      </ekz>
      <ekz>
        tiuj spektakloj plivigligis la <tld/>ojn kaj kiam oni ne <tld/>is, la enuo estis
        tiom supermezura
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXX</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi volas <klr>[&#8230;]</klr> instigi vin, bone kaj senpartie prepari&gcirc;i por la
        venontaj <tld/>oj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sepa Kongreso Esperantista en Antwerpen en la 21a de a&ubreve;gusto
          1911</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="debat.0o">debato</ref>,
      <ref cel="disput.0o">disputo</ref>,
      <ref cel="parol.inter0o">interparolo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_y;&c_s;&c_k;&c_u;&c_s;&c_ib;&c_ja;</trd>,
    <trd>&c_a;&c_b;&c_m;&c_je;&c_r;&c_k;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="de">Diskussion</trd>
  <trd lng="en">discussion</trd>
</drv>




<drv mrk="diskut.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="diskut.pri0i.KOMUNE">
    <dif>
      <ref tip="super" cel="ekzame.0i.esplori">Ekzameni</ref> opinion, 
      aferon, temon, inter&scirc;an&gcirc;ante argumentojn pri malsamaj vidpunktoj:
      <ekz>
        la filozofio pri<tld/>is la demandon &ccirc;iuflanke
        <fnt>
          <bib>Ret</bib>
          <lok>Aldono</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi decidis uzi vian kunsidon kaj peti vin pri<tld/>i mian proponon
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        dum vi pri<tld/>as vian aferon, mi preparos la teon
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili interparolis kiel du bonaj amikoj, kiuj duonla&ubreve;te pri<tld/>as interesan temon
        <fnt>
          <bib>HsT</bib>
          <lok>Feli&ccirc;a homo</lok>
        </fnt>;
      </ekz>
      <ekz>
        pri<tld/>i la familiajn nova&jcirc;ojn
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili komencis pri<tld/>i la belan domegon kaj &gcirc;ian posedantinon
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kontes.0i">kontesti</ref>,
      <ref cel="refut.0i">refuti</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_m;&c_ja;&c_r;&c_k;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">prodiskutovat</trd>
  <trdgrp lng="de">
    <trd>besprechen</trd>,
    <trd>ausdiskutieren</trd>,
    <trd>er&ouml;rtern</trd>
  </trdgrp>
  <trd lng="fr">discuter<klr tip="ind"> (tr.)</klr></trd>
  <trd lng="he">&lamed;&#1464;&dalet;&vav;&#1468;&fnun; &bet;</trd>
  <trd lng="pl">przedyskutowa&cacute;</trd>
  <trd lng="ru">&c_o;&c_b;&c_s;&c_u;&c_d;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">prediskutova&tcaron;</trd>
</drv>

<drv mrk="diskut.0ebla">
