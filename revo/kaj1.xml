<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
  <rad>kaj</rad>/o
</kap>
<drv mrk="kaj1.0o">
  <kap><tld/>o</kap>
  <snc mrk="kaj1.0o.MAR">
    <dif>
      Terbenkego, firmigita per muro, la&ubreve;longe de rivero a&ubreve;
      &ccirc;e la marbordo, uzata kiel strato a&ubreve; el&scirc;ipi&gcirc;ejo. 
    </dif>
    <trd lng="de">Kai</trd>
    <trdgrp lng="en">
      <trd>quay</trd>,
      <trd>pier</trd>,
      <trd>wharf</trd>
    </trdgrp>
    <trd lng="nl">kade</trd>
    <trdgrp lng="ru">
      <trd>&c_n;&c_a;&c_b;&c_je;&c_r;&c_je;&c_zh;&c_n;&c_a;&c_ja;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_s;&c_t;&c_a;&c_n;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_ch;&c_a;&c_l;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kaj1.0o.FER">
    <dif>
      Alta trotuaro konstruita la&ubreve;longe de fervojo
      por ebligi al la pasa&gcirc;eroj facilan en- kaj elvagoni&gcirc;on:
      <ekz>la staciaj <tld/>oj.</ekz>
    </dif>
    <trd lng="de">Bahnsteig</trd>
    <trd lng="en">platform</trd>
    <trd lng="tr">peron</trd>
    <trd lng="nl">perron</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_je;&c_r;&c_r;&c_o;&c_n;</trd>,
      <trd>&c_zh;&c_je;&c_l;&c_je;&c_z;&c_n;&c_o;&c_d;&c_o;&c_r;&c_o;&c_zh;&c_n;&c_a;&c_ja; &c_p;&c_l;&c_a;&c_t;&c_f;&c_o;&c_r;&c_m;&c_a;</trd>
    </trdgrp>
  </snc>
</drv>
</art>
