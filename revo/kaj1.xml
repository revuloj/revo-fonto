<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>1</ofc>
   <rad>kaj</rad>/o
</kap>
<drv mrk="kaj1.0o">
   <kap><tld/>o</kap>
   <snc mrk="kaj1.0o.KOMUNE">
     <dif>
       Teraso aran&gcirc;ita por facila aliro al veturiloj:
     </dif>
     <subsnc mrk="kaj1.0o.MAR">
       <uzo tip="fak">MAR</uzo>
       <dif>
         Teraso la&ubreve;longe de rivero a&ubreve; marbordo, uzata kiel 
<ref
         tip="vid" cel="sxip.el0ejo">el&scirc;ipejo</ref>.
       </dif>
       <trdgrp lng="be">
         <trd>&c_n;&c_a;&c_b;&c_ja;&c_r;&c_e;&c_zh;&c_n;&c_a;&c_ja;</trd>,
         <trd>&c_p;&c_r;&c_y;&c_s;&c_t;&c_a;&c_n;&c_mol;</trd>,
         <trd>&c_p;&c_r;&c_y;&c_ch;&c_a;&c_l;</trd>
       </trdgrp>
       <trd lng="de">Kai</trd>
       <trd lng="es">muelle</trd>
       <trd lng="fi">(ranta)<ind>laituri</ind></trd>
       <trdgrp lng="en">
         <trd>quay</trd>,
         <trd>pier</trd>,
         <trd>wharf</trd>
       </trdgrp>
       <trd lng="he">&mem;&zayin;&het;</trd>
       <trd lng="hu">rakpart</trd>
       <trd lng="pt">cais</trd>
       <trd lng="nl">kade</trd>
       <trdgrp lng="ru">
         <trd>&c_n;&c_a;&c_b;&c_je;&c_r;&c_je;&c_zh;&c_n;&c_a;&c_ja;</trd>,
         <trd>&c_p;&c_r;&c_i;&c_s;&c_t;&c_a;&c_n;&c_mol;</trd>,
         <trd>&c_p;&c_r;&c_i;&c_ch;&c_a;&c_l;</trd>
       </trdgrp>
        <trd lng="sv">kaj</trd>
     </subsnc>
     <subsnc mrk="kaj1.0o.FER">
       <uzo tip="fak">FER</uzo>
       <dif>
         Alta trotuaro konstruita la&ubreve;longe de fervojo
         por ebligi al la pasa&gcirc;eroj facilan en- kaj elvagoni&gcirc;on:
         <ekz>
           la trajno delonge staris &ccirc;e <tld/>o
           <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 12a, p.
70a</lok></fnt>.
         </ekz>
       </dif>
       <trdgrp lng="be">
         <trd>&c_p;&c_l;&c_ja;&c_t;&c_f;&c_o;&c_r;&c_m;&c_a;</trd>,
         <trd>&c_p;&c_e;&c_r;&c_o;&c_n;</trd>
       </trdgrp>
       <trd lng="de">Bahnsteig</trd>
       <trd lng="en">platform</trd>
       <trd lng="es">and&eacute;n</trd>
       <trd lng="fi">(asema)<ind>laituri</ind></trd>
       <trd lng="he">&resh;&tsadi;&yod;&fpe;</trd>
       <trd lng="hu">peron</trd>
       <trd lng="nl">perron</trd>
       <trd lng="pl">peron</trd>
       <trd lng="pt">plataforma</trd>
       <trdgrp lng="ru">
         <trd>&c_p;&c_je;&c_r;&c_r;&c_o;&c_n;</trd>,

<trd>&c_zh;&c_je;&c_l;&c_je;&c_z;&c_n;&c_o;&c_d;&c_o;&c_r;&c_o;&c_zh;&c_n;&c_a;&c_ja; 

&c_p;&c_l;&c_a;&c_t;&c_f;&c_o;&c_r;&c_m;&c_a;</trd>
       </trdgrp>
       <trdgrp lng="sv">
         <trd>perrong</trd>,
         <trd>plattform</trd>
       </trdgrp>
       <trd lng="tr">peron</trd>
     </subsnc>
   </snc>
   <trd lng="fr">quai</trd>
</drv>
</art>
