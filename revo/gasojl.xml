<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gasojl.xml,v 1.11 2022/04/30 08:42:40 revo Exp $">
<kap>
  <rad>gasojl</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="gasojl.0o">
  <kap><tld/>o</kap>
  <snc mrk="gasojl.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="dif" cel="ole.gas0o.KEM">Gasoleo</ref>:
      <ekz>
        la prezo de <tld/>o inkluzivas
        relative altan imposton
        <fnt>
         <aut>K. Kniivil&auml;</aut>,
         <vrk><url ref="http://www.kniivila.net/1987/gasojlon-au-gasogenojn/">
         Gasojlon a&ubreve; gasogenojn?</url></vrk>,
         <lok>1987</lok>
        </fnt>;
      </ekz>
      <ekz>
        el t.n. firma biomaso oni povas produkti <tld/>on kun alta biogena parto, ta&ubreve;gan
        por &ccirc;iuj motoroj
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2013/010881.php">Gasojlo el lignaj
          defala&jcirc;oj</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc><trdgrp lng="be">
    <trd>&c_g;&c_a;&c_z;&c_o;&c_j;&c_l;&c_mol;</trd>,
    <trd>&c_d;&c_y;&c_z;&c_e;&c_l;&c_mol;&c_n;&c_a;&c_je; &c_p;&c_a;&c_l;&c_ib;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Diesel</trd>
  <trd lng="en">diesel fuel</trd>
  <trd lng="cs">motorov&aacute; nafta</trd>
  <trdgrp lng="fr">
    <trd>diesel</trd>,
    <trd>gazole</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>olej nap&eogonek;dowy</trd>,
    <trd>ropa <klr>(pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>&oacute;leo diesel</trd>
  </trdgrp>
  <trd lng="sk">motorov&aacute; nafta</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_i;&c_z;&c_je;&#x43B;&#x44C;&#x43D;&c_je; &#x43F;&c_a;&#x43B;&c_i;&c_v;&#x43E;</trd>,
    <trd>&c_s;&#x43E;&#x43B;&#x44F;&c_r;&#x43A;&c_a;</trd>
  </trdgrp>
</drv>
</art>

