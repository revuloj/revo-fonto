<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pimp.xml,v 1.13 2023/09/03 12:45:07 revo Exp $">
<kap>
  <rad>pimp</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="pimp.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="pimp.0a.KOMUNE">
    <dif>
      <ref tip="super" cel="bel.0a">Bela</ref>, <ref tip="super" cel="placx.0a">pla&ccirc;a</ref>
      pro fre&scirc;eco, allogeco, vigleco:
      <ekz>
        la&ubreve; mia aspekto, mi ne estis glora ekzemplo de tiu tia pura kaj <tld/>a germana
        lando kaj de &gcirc;iaj &ccirc;iam tiel fre&scirc;e lavitaj kaj <tld/>aj enlo&gcirc;antoj
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        A&hcirc;, a&hcirc; ! &dash; Anstata&ubreve; <tld/>e // Ala&ubreve;djubili al la lum' // Ja licas nur sengrimpe
        // Langvori en mucida hum'
        <fnt>
          <bib>LSB</bib>
          <lok>Lamentado</lok>
        </fnt>;
      </ekz>
      <ekz>
        la eleganta piedo, kies artiko estas volvita de blanka banda&gcirc;o <klr>[&#x2026;]</klr>
        similis al <tld/>a lotusfloro sur la verda fono de la kovrilo
        <fnt>
          <aut>Shi Chengtai</aut>
          <vrk><url ref="&Monato;/1995/A950601.php">La kompensa kiso</url></vrk>
          <bib>Monato</bib>
          <lok>1995/06, p. 27</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj laborvesta&jcirc;oj estis de aparta fasono kaj aspektis e&ccirc; <tld/>e &ccirc;e li
        <fnt>
          <bib>Monato</bib>
          <aut>Lena Karpunina</aut>
          <vrk><url ref="&Monato;/2008/009339.php">En granda urbo</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        li vestis sin per sia diman&ccirc;a kompleto, siaj plej <tld/>a &ccirc;apelo, palto,
        kaproledaj gantoj, kaj nigraj &scirc;uoj
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2014/010918.php">Mirakloj</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="cxarm.0a">&ccirc;arma</ref>,
      <ref cel="elegan.0a">eleganta</ref>,      
      <ref cel="sxik.0a">&scirc;ika</ref>,
      <ref cel="vigl.0a.rapidmova">vigla</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>elegantn&iacute;</trd>,
    <trd>p&rcaron;ita&zcaron;liv&yacute;</trd>,
    <trd>sv&uring;dn&yacute;</trd>,
    <trd>vystrojen&yacute;</trd>,
    <trd>&scaron;varn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>adrett</trd>,
    <trd>fesch</trd>,
    <trd>flott</trd>,
    <trd>gepflegt</trd>,
    <trd>schnieke</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>dapper</trd>,
    <trd>spruce</trd>,
    <trd>stylish</trd>,
    <trd>natty</trd>
  </trdgrp>
  <trd lng="fr">pimpant</trd>
  <trdgrp lng="ja">
    <trd>&#x3044;&#x304D;&#x306A;</trd>,
    <trd>&#x8272;&#x3063;&#x307D;&#x3044;  [<ind>&#x3044;&#x308D;&#x3063;&#x307D;&#x3044;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>fleurig</trd>,
    <trd>elegant</trd>,
    <trd>koket</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>elegancki</trd>,
    <trd>strojny</trd>,
    <trd>wymuskany</trd>,
    <trd>wytworny</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>sedutora</trd>,
    <trd>atraente</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>p&ocirc;vabn&yacute;</trd>,
    <trd>v&aacute;biv&yacute;</trd>,
    <trd>&scaron;v&aacute;rny</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_sh;&c_i;&c_k;&c_a;&c_r;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_f;&c_r;&c_a;&c_n;&c_t;&c_ib;&c_v;&c_s;&c_mol;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_je;&c_l;&c_je;&c_gu;&c_a;&c_n;&c_t;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="pimp.0ulino">
  <kap>
    <tld/>ulino
  </kap>
  <snc mrk="pimp.0ulino.KOMUNE">
    <dif>
      <tld lit="P"/>a, vigla, ordinare juna, alloga ino:
      <ekz>
        estis <nom>swingin' London</nom>, la erotika &ccirc;efurbo de la mondo,
        prilo&gcirc;ata de sennombraj <tld/>ulinoj en miniaturaj jupoj
        <fnt>
          <aut>T. Steele</aut>
          <vrk>Konvinka Kamufla&jcirc;o</vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">flotte <ind>Biene</ind></trd>
  <trd lng="en">babe</trd>
  <trd lng="fr">bimbo</trd>
</drv>
</art>

