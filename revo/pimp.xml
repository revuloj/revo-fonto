<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>pimp</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="pimp.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="bel.0a">Bela</ref> pro spontanea, vigla
      fre&scirc;o:
      <ekz>
        &gcirc;iaj &ccirc;iam tiel fre&scirc;e lavitaj kaj <tld/>aj enlo&gcirc;antoj
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>unua libro, &ccirc;apitro
        16a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">pimpant</trd>
  <trdgrp lng="nl">
    <trd>fleurig</trd>,
    <trd>elegant</trd>,
    <trd>koket</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>sedutora</trd>,
    <trd>atraente</trd>
  </trdgrp>
</drv>
</art>

