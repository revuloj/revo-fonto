    <kap>
      <rad>morul</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="morul.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BIO</uzo>
  <snc>
    <dif>
      Embria statdio de plur&ccirc;elaj bestoj evolui&gcirc;anta el la <ref cel="zigot.0o">zigoto</ref>
      kiel morusforma buleto de 16 &gcirc;is 32 &ccirc;eloj:
      <ekz>
        niaj prauloj, do, estis sekve &ccirc;elo, <tld/>o, <ref cel="blastul.0o">blastulo</ref>, gastrulo
        <klr>[&#8230;]</klr>
        <fnt>
          <vrk><url
          ref="https://eo.wikisource.org/wiki/Pa%C4%9Do:P.-J._-_La_deveno_de_l%E2%80%99Homo,_1914,_Du_Homaranoj.pdf/31"
              >La deveno de l&#8217;Homo</url></vrk>
          <lok>p. 29</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Morula</trd>
    <trd lng="en">morula</trd>
    <trd lng="fr">morula</trd>
    <trd lng="pl">morula</trd>
    <trd lng="ru">&c_m;&c_o;&c_r;&c_u;&c_l;&c_a;</trd>
  </snc>
</drv>

  </art>

