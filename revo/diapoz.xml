<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: diapoz.xml,v 1.19 2021/03/08 15:14:34 revo Exp $">
<kap>
  <rad>diapozitiv</rad>/o
</kap>
<drv mrk="diapoz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      Pozitiva fotografa kopio sur filmo a&ubreve; vitro:
      <ekz>
        lumaj spektakloj, projekciataj de koloraj <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=28500">Pink Floyd</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Tibor Sekel</nom>j kiel edukisto efektivigis inter 7000 kaj 8000 prelegoj, plej ofte
        kun <tld/>oj pri siaj voja&gcirc;oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=7480">Tibor Sekelj</url></lok>
        </fnt>.
      </ekz> 
    </dif>
    <ref tip="super" cel="bild.lum0o">lumbildo</ref>
    <refgrp tip="vid">
      <ref cel="projek.0ajxo.FOT">projekcia&jcirc;o</ref>,
      <ref cel="projek.0ilo">projekciilo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_p;&c_a;&c_z;&c_y;&c_t;&c_y;&c_w;</trd>
  <trd lng="bg">&c_d;&c_i;&c_a;&c_p;&c_o;&c_z;&c_i;&c_t;&c_i;&c_v;</trd>
  <trd lng="ca">diapositiva</trd>
  <trd lng="cs">diapozitiv</trd>
  <trdgrp lng="de">
     <trd>Diapositiv</trd>,
     <trd>Dia</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>slide</trd>,
    <trd>transparency</trd>,
    <trd>diapositive</trd>
  </trdgrp>
  <trdgrp lng="es">
     <trd>diapositiva</trd>,
     <trd>transparencia</trd>
  </trdgrp>
  <trd lng="fr">diapositive</trd>
  <trd lng="he">&#x5E9;&#x5E7;&#x5D5;&#x5E4;&#x5D9;&#x5EA;</trd>
  <trdgrp lng="hu">
     <trd>dia</trd>,
     <trd>diapozit&iacute;v</trd>
  </trdgrp>
  <trd lng="nl">dia</trd>
  <trdgrp lng="pl">
    <trd>diapozytyw</trd>,
    <trd>slajd</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>diapositivo</trd>,
    <trd>slide</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_i;&c_a;&c_p;&c_o;&c_z;&c_i;&c_t;&c_i;&c_v;</trd>
  <trd lng="sk">diapozit&iacute;v</trd>
</drv>
<adm>
  Koherigi kun la jama "projekciajxo" kaj la enkondukota
  "lumbildo". <aut>MB</aut>
</adm>
</art>
