<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>diapozitiv</rad>/o
</kap>
<drv mrk="diapoz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
  <dif>
    Pozitiva fotografa kopio sur filmo a&ubreve; vitro.  
  </dif>
  <refgrp tip="vid">
    <ref cel="projek.0ajxo.FOT">projekcia&jcirc;o</ref>,
    <ref cel="projek.0ilo">projekciilo</ref>.
  </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_p;&c_a;&c_z;&c_y;&c_t;&c_y;&c_w;</trd>
  <trd lng="bg">&c_d;&c_i;&c_a;&c_p;&c_o;&c_z;&c_i;&c_t;&c_i;&c_v;</trd>
  <trdgrp lng="de">
     <trd>Diapositiv</trd>,
     <trd>Dia</trd>
  </trdgrp>
  <trdgrp lng="es">
     <trd>diapositiva</trd>,
     <trd>transparencia</trd>
  </trdgrp>
  <trd lng="fr">diapositive</trd>
  <trdgrp lng="hu">
     <trd>dia</trd>,
     <trd>diapozit&iacute;v</trd>
  </trdgrp>
  <trd lng="nl">dia</trd>
  <trdgrp lng="pl">
    <trd>diapozytyw</trd>,
    <trd>slajd</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>diapositivo</trd>,
    <trd>slide</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_i;&c_a;&c_p;&c_o;&c_z;&c_i;&c_t;&c_i;&c_v;</trd>
</drv>
<adm>
  Koherigi kun la jama "projekciajxo" kaj la enkondukota
  "lumbildo". <aut>MB</aut>
</adm>
</art>
