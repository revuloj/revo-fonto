  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="river.0o">Rivero</ref>, fluanta el 
      <ref tip="malprt" cel="franc.F0ujo">Francujo</ref>, tra 
      <ref tip="malprt" cel="belg.B0ujo">Belgujo</ref> al 
      <ref tip="malprt" cel="nederl.0o">Nederlando</ref>,
