</kap>

<drv mrk="dek.0">
   <kap><ofc>*</ofc><tld/></kap>
   <mlg>10</mlg>
   <snc mrk="dek.0.KAL">
     <dif>
        <ref tip="lst" cel="nombr.0o.MAT" lst="voko:nombroj" val="10">Nombro</ref>:
        duoble kvin:
       <ekz>
         mi havas <tld/> fingrojn;
       </ekz>
       <ekz>
         <tld/>o da homoj;
       </ekz>
      <ekz>
        pasis <tld/> jaroj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 16:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        du<tld/> jaroj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 31:41</lok>
        </fnt>;
      </ekz>
      <ekz>
        na&ubreve;cent <tld/> jaroj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 5:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>-sepa tago de la monato
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 7:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/> sikloj da oro
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 24:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        eble trovi&gcirc;os tie <tld/>
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 18:32</lok>
        </fnt>;
      </ekz>
      <ekz>
        malsa&gcirc;ulo malpacigis kaj <tld/> sa&gcirc;uloj repacigi ne povas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        pli zorgas unu patrino pri <tld/> infanoj, ol <tld/> infanoj pri unu patrino
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        unu vido ta&ubreve;gas pli ol <tld/> a&ubreve;doj
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
     </dif>
   </snc>
   <trd lng="af">tien</trd>
  <trd lng="am">&#x12A0;&#x1235;&#x122D;</trd>
  <trd lng="ar">&#x639;&#x634;&#x631;&#x629;</trd>
  <trd lng="be">&c_d;&c_z;&c_je;&c_s;&c_ja;&c_c;&c_mol;</trd>
   <trd lng="bg">&c_d;&c_je;&c_s;&c_je;&c_t;</trd>
   <trd lng="bo">&#xF56;&#xF45;&#xF74;&#xF0B;</trd>
   <trd lng="br">dek</trd>
   <trd lng="ca">deu</trd>
   <trd lng="cs">deset</trd>
   <trd lng="de">zehn</trd>
   <trd lng="en">ten</trd>
   <trd lng="es">diez</trd>
   <trd lng="fi">kymmenen</trd>
   <trd lng="fr">dix</trd>
   <trd lng="fy">tsien</trd>
   <trd lng="he">&ayin;&shin;&resh;</trd>
   <trd lng="hi">&#x926;&#x938; [<ind>das</ind>]</trd>
   <trd lng="hu">t&iacute;z</trd>
   <trd lng="id">se<ind>puluh</ind></trd>
   <trd lng="io">dek</trd>
   <trd lng="it">dieci</trd>
   <trd lng="ja">&#x5341; [<ind>&#x3058;&#x3085;&#x3046;</ind>]</trd>
   <trd lng="ku">deh</trd>
   <trd lng="ne">&#x926;&#x938;</trd>
  <trd lng="nl">tien</trd>
   <trd lng="pl">dziesi&eogonek;&cacute;</trd>
   <trd lng="pt">dez</trd>
   <trd lng="ru">&c_d;&c_je;&c_s;&c_ja;&c_t;&c_mol;</trd>
   <trd lng="sk">desa&tcaron;</trd>
   <trd lng="sq">dhjet&#xEB;</trd>
  <trd lng="sv">tio</trd>
   <trd lng="tr">on</trd>
   <trd lng="vo">deg</trd>
   <trd lng="zh">&#x5341; [<ind>sh&iacute;</ind>]</trd>
</drv>

<drv mrk="dek.0a">
   <kap><tld/>a</kap>
   <snc mrk="dek.0a.KAL">
    <dif>
      <tld lit="D"/>aranga, venanta post na&ubreve; anta&ubreve;aj:
      <ekz>
        je la <tld/>a horo matene
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a tago de la monato
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 25:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        la akvo konstante malplii&gcirc;adis &gcirc;is la <tld/>a monato
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 8:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        la popolo ripozu &ccirc;iun sepan tagon, ne &ccirc;iun <tld/>an
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>an de Novembro de la jaro 1759 Dio donacis al ni filon
        <fnt>
          <bib>Fab4</bib>
          <lok>Malnova pre&gcirc;eja sonorilo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="deka.0">deka-</ref>
  <trd lng="be">&c_d;&c_z;&c_ja;&c_s;&c_ja;&c_t;&c_y;</trd>
  <trd lng="br">dekvet</trd>
  <trdgrp lng="ca">
     <trd>des&eacute;</trd>,
     <trd>d&egrave;cim</trd>
   </trdgrp>
  <trd lng="cs">des&aacute;t&yacute;</trd>
  <trd lng="de">zehnte</trd>
  <trd lng="es">d&eacute;cimo</trd>
  <trd lng="fi">kymmenes</trd>
  <trd lng="fr">dixi&egrave;me</trd>
  <trd lng="he">&ayin;&shin;&yod;&resh;&yod;</trd>
  <trd lng="nl">tiende</trd>
  <trd lng="pl">dziesi&aogonek;ty</trd>
  <trd lng="pt">d&eacute;cimo<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="ru">&c_d;&c_je;&c_s;&c_ja;&c_t;&c_y;&c_j;</trd>
  <trd lng="sk">desiaty</trd>
  <trd lng="zh">&#x7B2C;&#x5341; [<ind>d&igrave;sh&iacute;</ind>]</trd>
</drv>

<drv mrk="dek.0e">
   <kap><tld/>e</kap>
   <snc mrk="dek.0e.KAL">
     <dif>
      <tld lit="D"/>arange, post na&ubreve; anta&ubreve;aj:
      <ekz>
        Granda Ursa Lago estas <klr>[&#x2026;]</klr> la <tld/>e plej granda lago de la mondo
        <fnt>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Granda_Slavia_Lago">Granda Sklava Lago -
          Vikipedio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Nov-Hamp&scirc;iro estas la kvine plej malgranda kaj la <tld/>e <klr>[mal]</klr>plej
        <klr>[&#x2026;]</klr> populata de la 50 sub&scirc;tatoj
        <fnt>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Novhamp%C5%9Diro">Nov-Hamp&scirc;iro -
          Vikipedio</url></vrk>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trd lng="br">da <ind>zekvet</ind></trd>
  <trd lng="ca">en des&eacute; lloc</trd>
  <trd lng="cs">za des&aacute;t&eacute;</trd>
  <trd lng="de">zehntens</trd>
  <trd lng="fi">kymmenenneksi</trd>
  <trd lng="fr">dixi&egrave;mement</trd>
  <trd lng="nl">ten <ind>tiende</ind></trd>
  <trdgrp lng="pl">
     <trd>na <ind>dziesi&aogonek;tym</ind> miejscu</trd>,
     <trd>na <ind>dziesi&aogonek;tej</ind> pozycji</trd>,
     <trd>po <ind>dziesi&aogonek;te</ind></trd>
   </trdgrp>
  <trd lng="pt">em <ind>d&eacute;cimo</ind> lugar<klr tip="ind">
   (adv.)</klr></trd>
  <trd lng="ru">&c_v;-&c_d;&c_je;&c_s;&c_ja;&c_t;&c_y;&c_h;</trd>
  <trd lng="sk">po desiate</trd>
</drv>

<drv mrk="dek.0ono">
  <kap>
    <tld/>ono
  </kap>
  <snc>
    <dif>
      La <tld/>a parto de io:
      <ekz>
        omero estas <tld/>ono de efo
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 16:36</lok>
        </fnt>;
      </ekz>
      <ekz>
        el viaj &scirc;afoj li prenos <tld/>onon
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 8:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi oferos al Vi <tld/>onon
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 28:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        nu ja, anta&ubreve;e mi okupi&gcirc;is precipe pri enhavo kaj devis uzi nur <tld/>onon de mia
        labortempo por teknikaj aferoj
        <fnt>
          <bib>Monato</bib>
          <lok><url ref="http://steloj.de/esperanto/monato2/007058.html">http://steloj.de/esperanto/monato2/007058.html</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="deci.0">deci-</ref>
  </snc>
  <trd lng="de">Zehntel</trd>
  <trd lng="he">&#x5E2;&#x5B2;&#x5E9;&#x5C2;&#x5B4;&#x5D9;&#x5E8;&#x5B4;&#x5D9;&#x5EA;</trd>
</drv>


<drv mrk="dek.0onajxo">
  <kap><tld/>ona&jcirc;o</kap>
  <snc mrk="dek.0onajxo.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="impost.0o.EKON">Imposto</ref>
      plejofte religia, origine egala al dekono de rikolto,
      de enspezo...;  <ref tip="sin" cel="dism.0o.EKON">dismo</ref>.
    </dif>
      <subsnc mrk="dek.0onajxo.BIB">
        <uzo tip="fak">BIB</uzo>
        <dif>
          En antikva judismo, imposto pagata per natura&jcirc;o al la templo 
          kaj al la pastroj:
          <ekz>
            <nom>Abram</nom> donis al li <tld/>ona&jcirc;on el &ccirc;io
            <fnt>
              <bib>MT</bib>
              <lok>Genezo 14:20</lok>
            </fnt>;
          </ekz>
          <ekz>
            &ccirc;iu <tld/>ona&jcirc;o el la tero, el la semita&jcirc;o de la tero, el 
            la fruktoj de la arboj apartenas al la Eternulo
            <fnt><bib>MT</bib>, <lok>&Lev; 27:30</lok></fnt>
          </ekz>
        </dif>
      </subsnc>
      <subsnc mrk="dek.0onajxo.KRI">
        <uzo tip="fak">KRI</uzo>
        <dif>
          Imposto pagata per natura&jcirc;o a&ubreve; per mono al
          iu eklezio:
          <ekz>
            kial pastoroj frostigas la rilaton kun adeptoj, kiuj ne plu 
            kapablas doni multe da <tld/>ona&jcirc;o?
            <fnt>
              <aut>Gbeglo K.</aut>,
              <vrk><url ref="https://www.monato.be/2010/009910.php">
              Ventreligio, dua r aldoni&gcirc;u</url></vrk>,
              <bib>Monato</bib>,
              <lok>2010:01, p. 20a</lok>
            </fnt>
          </ekz>
        </dif>
            <trd lng="de">Kirchenzehnter</trd>
      </subsnc>
  </snc>
  <trd lng="de">Zehnter</trd>
  <trd lng="fr">d&icirc;me</trd>
  <trd lng="pl">dziesi&eogonek;cina</trd>
</drv>

<drv mrk="dek.0uma">
   <kap><tld/>uma</kap>
   <snc mrk="dek.0uma.MAT">
     <uzo tip="fak">MAT</uzo>
     <uzo tip="fak">KOMP</uzo>
     <fnt><bib>MatTerm</bib><lok>p. 9</lok></fnt>
     <refgrp tip="vid">
       <ref cel="um.0.MAT">-um</ref>,
       <ref cel="ses.dek0uma">deksesuma</ref>
     </refgrp>     
   </snc>
   <snc mrk="dek.0uma.sci">
     <uzo tip="fak">SCI</uzo>
     <dif>
       Sistemo de mezurunuoj en kiu la pli grandaj unuoj estas
       <tld/>obloj (a&ubreve; centobloj, milobloj a&ubreve; aliaj obloj
       a&ubreve; onoj
       per potenco de <tld/>):
       <ref tip="dif" cel="jota.0.MEZ">jota-</ref> Y- 10<sup>24</sup>,
       <ref tip="dif" cel="zeta.0">zeta-</ref> Z- 10<sup>21</sup>,
       <ref tip="dif" cel="eksa.0.MEZ">eksa-</ref> E- 10<sup>18</sup>,
       <ref tip="dif" cel="peta.0.MEZ">peta-</ref> P- 10<sup>15</sup>,
       <ref tip="dif" cel="tera.0.MEZ">tera-</ref> Y- 10<sup>12</sup>,
       <ref tip="dif" cel="giga.0">giga-</ref> G- 10<sup>9</sup>,
       <ref tip="dif" cel="mega.0">mega-</ref> M- 10<sup>6</sup>,
       <ref tip="dif" cel="kilo.0">kilo-</ref> k- 10<sup>3</sup>,
       <ref tip="dif" cel="hekto.0">hekto-</ref> h- 10<sup>2</sup>,
       <ref tip="dif" cel="deka.0">deka-</ref> da- 10<sup>1</sup>,
       <ref tip="dif" cel="deci.0">deci</ref> d- 10<sup>-1</sup>,
       <ref tip="dif" cel="centi.0">centi-</ref> c- 10<sup>-2</sup>,
       <ref tip="dif" cel="mili1.0.milono">mili-</ref> m-
       10<sup>-3</sup>,
       <ref tip="dif" cel="mikro.0">mikro-</ref> &#xB5;- 10<sup>-6</sup>,
       <ref tip="dif" cel="nano.0">nano-</ref> n- 10<sup>-9</sup>,
       <ref tip="dif" cel="piko.0">piko-</ref> p- 10<sup>-12</sup>,
       <ref tip="dif" cel="femto.0">femto-</ref> f- 10<sup>-15</sup>,
       <ref tip="dif" cel="ato.0">ato-</ref> a- 10<sup>-18</sup>,
       <ref tip="dif" cel="zepto.0">zepto-</ref> z- 10<sup>-21</sup>,
       <ref tip="dif" cel="jokto.0">jokto-</ref> y- 10<sup>-24</sup>.
       <ekz>
         la metra sistemo estas <tld/>uma:
         1m = 10dm = 100cm = 1000mm;
       </ekz>
       <ekz>
         de post 1971 la britoj uzas <tld/>uman monsistemon.
       </ekz>
     </dif>
   </snc>
   <ref tip="sin" cel="decima.0a">decimala</ref>
   <trd lng="af">desimale</trd>
  <trd lng="am">&#x12A0;&#x1235;&#x122D;&#x12EE;&#x123D;</trd>
  <trd lng="ar">&#x627;&#x644;&#x639;&#x634;&#x631;&#x64A;</trd>
  <trd lng="be">&c_d;&c_z;&c_je;&c_s;&c_ja;&c_t;&c_k;&c_o;&c_v;&c_y;</trd>
   <trd lng="br">degel</trd>
   <trd lng="ca">decimal <klr>(numeraci&oacute;)</klr></trd>
   <trd lng="de">dezimal</trd>
   <trd lng="en">decimal</trd>
   <trd lng="es">decimal</trd>
   <trd lng="fi">kymmenkantainen</trd>
   <trd lng="fr">d&eacute;cimal</trd>
   <trd lng="he">&ayin;&shin;&resh;&vav;&nun;&yod;</trd>
   <trdgrp lng="hu">
     <trd>tizes</trd>,
     <trd>decim&aacute;lis</trd>
   </trdgrp>
   <trd lng="id">desimal</trd>
   <trdgrp lng="nl">
     <trd>tiendelig</trd>,
     <trd>decimaal</trd>
   </trdgrp>
   <trd lng="pl">dziesi&eogonek;tny</trd>
   <trd lng="pt">decimal<klr tip="ind"> (adj.)</klr></trd>
   <trd lng="ru">&c_d;&c_je;&c_s;&c_ja;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
   <trd lng="sk">desiatkov&yacute;</trd>
   <trd lng="sq">dhjetor</trd>
  <trd lng="zh">&#x5C0F;&#x6570; <klr>(tr. &#x5C0F;&#x6578;)</klr> [<ind>xi&#x1CE;osh&ugrave;</ind>]</trd>
  <trd lng="sk">desiatkov&yacute; <klr>(syst&eacute;m)</klr></trd>
  <trdgrp lng="cs">
    <trd>decim&aacute;ln&iacute;</trd>,
    <trd>desetinn&yacute; (&ccaron;&iacute;slo)</trd>,
    <trd>des&iacute;tkov&yacute; <klr>(syst&eacute;m)</klr></trd>
  </trdgrp>
</drv>


<drv mrk="dek.jar0o">
  <kap>jar<tld/>o</kap>
  <snc mrk="dek.jar0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Periodo de <tld/> jaroj:
      <ekz>
        en la tria jar<tld/>o de la lasta jarcento;
      </ekz>
      <ekz>
        la raportitaj okazinta&jcirc;oj ja okazis anta&ubreve; pli ol tri jar<tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Werner Schad</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006868.html">Venezuelo: Inter maro kaj
          arbarego</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kuketoj, kiujn mi konis dum jar<tld/>oj, subite i&gcirc;is, en novaj paka&jcirc;oj,
        Dinomordoj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9402-filmotondro.txt">Filmotondro</url></vrk>
          <lok>94/02</lok>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>decenni</trd>,
    <trd>d&egrave;cada</trd>
  </trdgrp>
  <trd lng="de">Jahrzehnt</trd>
  <trd lng="en">decade</trd>
  <trdgrp lng="es">
    <trd>decenio</trd>,
    <trd>d&eacute;cada</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;cennie</trd>
  <trd lng="fy">desennium</trd>
  <trdgrp lng="id">
    <trd>dekade</trd>,
    <trd>dasawarsa</trd>
  </trdgrp>
  <trd lng="io">yardeko</trd>
  <trd lng="nl">decennium</trd>
  <trd lng="vo">degyel</trd>
</drv>
</art>

