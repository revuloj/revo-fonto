<drv mrk="dek.0a">
   <kap><tld/>a</kap>
   <snc mrk="dek.0a.KAL">
    <dif>
      <tld lit="D"/>aranga, venanta post na&ubreve; anta&ubreve;aj:
      <ekz>
        je la <tld/>a horo matene
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a tago de la monato
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 25:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        la akvo konstante malplii&gcirc;adis &gcirc;is la <tld/>a monato
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 8:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        la popolo ripozu &ccirc;iun sepan tagon, ne &ccirc;iun <tld/>an
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>an de Novembro de la jaro 1759 Dio donacis al ni filon
        <fnt>
          <bib>Fab4</bib>
          <lok>Malnova pre&gcirc;eja sonorilo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="deka.0">deka-</ref>
  <trd lng="be">&c_d;&c_z;&c_ja;&c_s;&c_ja;&c_t;&c_y;</trd>
  <trd lng="br">dekvet</trd>
  <trdgrp lng="ca">
     <trd>des&eacute;</trd>,
     <trd>d&egrave;cim</trd>
   </trdgrp>
  <trd lng="cs">des&aacute;t&yacute;</trd>
  <trd lng="de">zehnte</trd>
  <trd lng="es">d&eacute;cimo</trd>
  <trd lng="fi">kymmenes</trd>
  <trd lng="fr">dixi&egrave;me</trd>
  <trd lng="he">&ayin;&shin;&yod;&resh;&yod;</trd>
  <trd lng="ja">&#x5341;&#x756A;&#x76EE;&#x306E;  <pr>&#x3058;&#x3085;&#x3046;&#x3070;&#x3093;&#x3081;&#x306E;</pr></trd>
