<kap>
  <ofc>4</ofc>
  <rad>splen</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="splen.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Malsana stato de la animo karakterizata per konstanta enuo kaj
      indiferenteco al &ccirc;io:
      <ekz>
	maljunaj personoj, kiuj
	naive kredas &ndash; jen venos komunistoj al la potenco, kaj foriros de
	ili re&ubreve;matismo, <tld/>o, sendormeco<fnt><aut>Lev</aut>:
	<vrk>Komunismo kaj feli&ccirc;o de popolo</vrk>,
	<bib>Monato</bib><lok>jaro 1997a, numero 10a, p.
	22a</lok></fnt>...
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="deprim.0o">deprimo</ref>,
      <ref cel="nigr.0a">nigra</ref>,
      <ref cel="melank.0o">melankolio</ref>
    </refgrp>
    <refgrp tip="ant">
      <ref cel="entuzi.0o">entuziasmo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_l;&c_ib;&c_n;</trd>,
    <trd>&c_h;&c_a;&c_n;&c_d;&c_r;&c_a;</trd>,
    <trd>&c_s;&c_m;&c_u;&c_t;&c_a;&c_k;</trd>,
    <trd>&c_t;&c_u;&c_g;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">espl&iacute;n</trd>
  <trdgrp lng="cs">
    <trd>nuda</trd>,
    <trd>omrzelost</trd>,
    <trd>spl&iacute;n</trd>
  </trdgrp>
  <trd lng="de">Schwermut</trd>
  <trdgrp lng="en">
    <trd>blues</trd>,
    <trd>low spirits</trd>,
    <trd>melancholy</trd>,
    <trd>depression</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>espl&iacute;n</trd>,
    <trd>bajo estado de &aacute;nimo</trd>,
    <trd>melancol&iacute;a</trd>,
    <trd>sentimiento de tedio de la vida</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_m;&a_l;&a_A;&a_l;</trd>,
    <trd>&a_A;&a_n;&a_d;&a_w;&a_h;</trd>,
    <trd>&a_d;&a_l;&a_t;&a_n;&f_g;&f_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ac&eacute;die</trd>,
    <trd>mal de vivre</trd>,
    <trd>spleen</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;letunts&aacute;g</trd>,
    <trd>k&ouml;z&ouml;ny</trd>,
    <trd>spleen</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6182;&#x9B31;  <pr>&#x3086;&#x3046;&#x3046;&#x3064;</pr></trd>,
    <trd>&#x3075;&#x3055;&#x304E;&#x3053;&#x307F;</trd>,
    <trd>&#x843D;&#x3061;&#x8FBC;&#x3093;&#x3067;&#x7121;&#x6C17;&#x529B;&#x306A;&#x6C17;&#x5206; 
    <pr>&#x304A;&#x3061;&#x3053;&#x3093;&#x3067;&#x3080;&#x304D;&#x308A;&#x3087;&#x304F;&#x306A;&#x304D;&#x3076;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">zwaarmoedigheid</trd>
  <trdgrp lng="pl">
    <trd>splin</trd>,
    <trd>apatia</trd>,
    <trd>nuda</trd>,
    <trd>ospa&lstroke;o&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_l;&c_i;&c_n;</trd>,
    <trd>&c_h;&c_a;&c_n;&c_d;&c_r;&c_a;</trd>,
    <trd>&c_t;&c_o;&c_s;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nuda</trd>,
    <trd>omrzenos&tcaron;</trd>,
    <trd>skleslos&tcaron;</trd>,
    <trd>spleen</trd>,
    <trd>svetob&ocirc;&lcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43F;&#x43B;&c_ib;&#x43D;</trd>,
    <trd>&#x43D;&c_u;&c_d;&#x44C;&c_g;&c_a;</trd>,
    <trd>&c_h;&c_a;&#x43D;&c_d;&c_r;&c_a;</trd>,
    <trd>&c_t;&c_u;&c_g;&c_a;</trd>,
    <trd>&c_zh;&c_u;&c_r;&c_b;&c_a;</trd>,
    <trd>&#x43C;&c_a;&#x43B;&#x43E;&c_v;&c_zh;&c_i;&c_v;. &c_s;&c_je;&#x43B;&c_je;&c_z;&c_ib;&#x43D;&#x43A;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x60C6;&#x6005; <pr>ch&#xF3;uch&#xE0;ng</pr></trd>,
    <trd>&#x60C6;&#x60B5; <pr>ch&#xF3;uch&#xE0;ng</pr></trd>,
    <trd>&#x60B2;&#x6101; <pr>b&#x113;ich&#xF3;u</pr></trd>,
    <trd>&#x610A; <pr>b&#xEC;</pr></trd>,
    <trd>&#x61D1; <pr>m&#xE8;n</pr></trd>,
    <trd>&#x61E3; <pr>m&#xE8;n</pr></trd>
  </trdgrp>
</drv>
