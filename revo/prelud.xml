<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: prelud.xml,v 1.24 2023/09/03 12:45:11 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>prelud</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="prelud.0o">
  <kap><tld/>o</kap>
  <snc mrk="prelud.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Ordinare mallonga, enkonduka
      <ref tip="super" cel="muzik.0ajxo.MUZ">muzika&jcirc;o</ref>,
      ofte por unu muzikilo, kun libera enhavo,
      anka&ubreve; memstare ludebla:
      <ekz>
        la <ctl><tld/>o al Posttagmezo de Fa&ubreve;no</ctl>,
        de Debussy, kaj la poemo sur kiu &gcirc;i bazi&gcirc;as,
        estigas la same revigan, eteran, sed ne malpli
        pasian senton
        <fnt>
          <aut>GrizaLeono</aut>
          <vrk><url ref="https://tatoeba.org/epo/sentences/show/1436497">
          Tatoeba, frazo n-ro 1436497</url></vrk>
          <lok>2012-02-15</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="uvertu.0o">uverturo</ref>
  </refgrp>
  <trd lng="be">&c_p;&c_r;&c_e;&c_l;&c_ju;&c_d;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_p;&c_r;&c_je;&c_l;&c_ju;&c_d;&c_i;&c_ja;</trd>
  <trd lng="ca">preludi</trd>
  <trdgrp lng="de">
    <trd>Pr&auml;ludium</trd>,
    <trd>Vorspiel</trd>
  </trdgrp>
  <trd lng="en">prelude</trd>
  <trdgrp lng="fr">
    <trd>pr&eacute;lude</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>prel&uacute;dium</trd>,
    <trd>el&odblac;j&aacute;t&eacute;k</trd>
  </trdgrp>
  <trd lng="it">preludio<klr> (mus.)</klr></trd>
  <trd lng="ja">&#x524D;&#x594F;&#x66F2;  [<ind>&#x305C;&#x3093;&#x305D;&#x3046;&#x304D;&#x3087;&#x304F;</ind>]</trd>
  <trdgrp lng="nl">
    <trd>prelude</trd>,
    <trd>voorspel</trd>
  </trdgrp>
  <trd lng="pl">preludium</trd>
  <trd lng="pt">prel&uacute;dio</trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_l;&c_ju;&c_d;&c_i;&c_ja;</trd>
  <trdgrp lng="sv">
    <trd>preludium</trd>,
    <trd>f&ouml;rspel</trd>
  </trdgrp>
  <trd lng="sk">predohra</trd>
  <trdgrp lng="cs">
    <trd>preludium</trd>,
    <trd>p&rcaron;edehra</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x440;&#x435;&#x43B;&#x44E;&#x434;&#x456;&#x44F;</trd>
</drv>

<drv mrk="prelud.0eal">
  <kap><tld/>e al</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="prelud.0eal.TEMP">
    <dif>
      <ref tip="super" cel="antaux.0.tempe">Anta&ubreve;<sncref/></ref>,
      <ref tip="vid" cel="prepar.0eal.KOMUNE">prepare al</ref>:
      <ekz>
        grupoj da soldatoj kaj amikoj <klr>...</klr> dise staris 
        parolante, foje serioze, foje kun afekta gajo, <tld/>e al 
        adia&ubreve;o
        <fnt>
          <aut>J. Francis</aut>
          <vrk>La Granda Kaldrono</vrk>
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
</drv>
</art>
