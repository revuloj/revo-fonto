<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gros.xml,v 1.34 2025/03/21 20:27:41 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>gros</rad>/o
<fnt><bib>UV</bib></fnt></kap>

<drv mrk="gros.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gros.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Frukto de <tld/>ujo:
      <ekz>
        konfita&jcirc;oj, preparitaj en la pasinta jaro kaj zorge fermitaj, sur iliaj paperaj
        kovriloj <nom>Feni&ccirc;ka</nom> propramane skribis per grandaj literoj: &leftquot;<tld/>oj&rightquot;
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Maria</nom> pa&scirc;etadis inter la berarbedoj, gustumante ru&gcirc;ajn, blankajn kaj nigrajn
        ribojn kaj la plej dol&ccirc;ajn <tld/>ojn
        <fnt>
          <aut>Jouko Lindstedt</aut>,
          <vrk><url ref="&Monato;/2000/006918.php">La helikoj</url></vrk>,
          <bib>Monato</bib>,
          <lok>2000/07, p. 16</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="../bld/gros.jpg">bran&ccirc;o kun <tld/>oj</bld>
  </snc>
  <trd lng="be">&c_a;&c_g;&c_r;&c_e;&c_s;&c_t;<klr> (&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="bg">&c_c;&c_a;&c_r;&c_i;&c_g;&c_r;&c_a;&c_d;&c_s;&c_k;&c_o; &c_g;&c_r;&c_o;&c_z;&c_d;&c_je;</trd>
  <trd lng="ca">agrass&oacute;</trd>
  <trd lng="de">Stachelbeere</trd>
  <trd lng="en">gooseberry</trd>
  <trdgrp lng="fa">
    <trd>&f_g;&a_A;&a_l;&a_sh;&zwnj;&a_A;&a_n;&f_g;&a_w;&a_r;</trd>,
    <trd>&a_A;&a_n;&f_g;&a_w;&a_r;&a_f;&a_r;&a_n;&f_g;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">groseille<klr> (&agrave; maquereau)</klr></trd>
  <trdgrp lng="hu">
    <trd>egres</trd>,
    <trd>k&ouml;szm&eacute;te</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd/>,
    <trd>&#12464;&#12540;&#12474;&#12505;&#12522;&#12540;&#12398;&#23455;  <pr>&#12464;&#12540;&#12474;&#12505;&#12522;&#12540;&#12398;&#12415;</pr></trd>,
    <trd>&#12473;&#12464;&#12522;&#12398;&#23455;  <pr>&#12473;&#12464;&#12522;&#12398;&#12415;</pr></trd>
  </trdgrp>
  <trd lng="pl">agrest</trd>
  <trd lng="pt">groselha</trd>
  <trd lng="ru">&c_k;&c_r;&c_y;&c_zh;&c_o;&c_v;&c_n;&c_i;&c_k; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="sk">egre&scaron;</trd>
  <trdgrp lng="cs">
    <trd>angre&scaron;t</trd>,
    <trd>srstka <klr>(ke&rcaron;)</klr></trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#37259;&#26647; <pr>c&ugrave;l&igrave;</pr></trd>,
    <trd>&#40285;&#33683; <pr>&eacute;m&eacute;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="gros.0ujo">
  <kap><tld/>ujo,
    <var><kap><tld/>arbeto</kap></var>
  </kap>
  <snc mrk="gros.0ujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:botanikaj_specioj">Specio</ref>
      de <ref tip="super" cel="rib.0o">ribo</ref>,
      kun grandaj acidaj, ofte haraj beroj,
      uzataj en konfita&jcirc;o ktp
      (<trd lng="la">Ribes uva-crispa</trd>):
      <ekz>
        <nom>Knut</nom> kaj Anjo, <klr>[&#8230;]</klr> rampadis unu al la alia tra sub la <tld/>arbeta&jcirc;oj
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        malfeli&ccirc;e, li pu&scirc;is sekan <tld/>-arbeton: &gcirc;ardenistino kvaza&ubreve; ektimis brueton
        <fnt>
          <bib>SrT</bib>
          <lok>Libro III.</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_g;&c_r;&c_e;&c_s;&c_t; <klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trd lng="ca">agrass&oacute;</trd>
  <trdgrp lng="cs">
    <trd>angre&scaron;t</trd>,
    <trd>srstka <klr>(ke&rcaron;)</klr></trd>
  </trdgrp>
  <trd lng="de">Stachelbeerstrauch</trd>
  <trd lng="en">gooseberry</trd>
  <trd lng="fa">&a_d;&a_r;&a_kh;&a_t;&f_ch;&a_h;&a_hamza_sure; <ind>&f_g;&a_A;&a_l;&a_sh;&zwnj;&a_A;&a_n;&f_g;&a_w;&a_r;</ind></trd>
  <trd lng="fr">groseiller <klr>(&agrave; maquereau)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#12464;&#12540;&#12474;&#12505;&#12522;&#12540;</trd>,
    <trd>&#12475;&#12452;&#12520;&#12454;&#12473;&#12464;&#12522;</trd>
  </trdgrp>
  <trd lng="nl">kruisbes</trd>
  <trd lng="pl">agrest<klr> (krzew)</klr></trd>
  <trd lng="pt">groselheira</trd>
  <trd lng="ru">&c_k;&c_r;&c_y;&c_zh;&c_o;&c_v;&c_n;&c_i;&c_k; <klr>(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">egre&scaron;  <klr>(strom&ccaron;ek)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_a;&c_gu;&c_r;&c_u;&c_s;</trd>,
    <trd>&c_k;&c_u;&c_shch;&nbsp;&c_a;&c_gu;&c_r;&c_u;&c_s;&c_u;</trd>
  </trdgrp>
</drv>
</art>
