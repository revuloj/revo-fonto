<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>gros</rad>/o
</kap>

<drv mrk="gros.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Frukto de <tld/>ujo.
    </dif>
  </snc>
  <bld lok="../bld/gros.jpg">bran&ccirc;o kun <tld/>oj</bld>
  <trd lng="be">&c_a;&c_g;&c_r;&c_e;&c_s;&c_t;<klr> (&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="bg">&c_c;&c_a;&c_r;&c_i;&c_g;&c_r;&c_a;&c_d;&c_s;&c_k;&c_o; &c_g;&c_r;&c_o;&c_z;&c_d;&c_je;</trd>
  <trd lng="de">Stachelbeere</trd>
  <trd lng="en">gooseberry</trd>
  <trdgrp lng="fa">
    <trd>&f_g;&a_A;&a_l;&a_sh;&zwnj;&a_A;&a_n;&f_g;&a_w;&a_r;</trd>,
    <trd>&a_A;&a_n;&f_g;&a_w;&a_r;&a_f;&a_r;&a_n;&f_g;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">groseille</trd>
  <trdgrp lng="hu">
    <trd>egres</trd>,
    <trd>k&ouml;szm&eacute;te</trd>
  </trdgrp>
  <trd lng="pl">agrest</trd>
  <trd lng="pt">groselha</trd>
  <trd lng="ru">&c_k;&c_r;&c_y;&c_zh;&c_o;&c_v;&c_n;&c_i;&c_k;
  <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
</drv>

<drv mrk="gros.0ujo">
  <kap><tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="rib.0o">ribo</ref>
      (<trd lng="la">Ribes uvacrispa</trd>),
      kun grandaj acidaj, ofte haraj beroj,
      uzataj en konfita&jcirc;o ktp.
    </dif>
    <ref tip="super" cel="rib.0o">ribo</ref>
  </snc>
  <trd lng="be">&c_a;&c_g;&c_r;&c_e;&c_s;&c_t;
  <klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trd lng="de">Stachelbeerstrauch</trd>
  <trd lng="fa">&a_d;&a_r;&a_kh;&a_t;&f_ch;&a_h;&a_hamza_sure; <ind>&f_g;&a_A;&a_l;&a_sh;&zwnj;&a_A;&a_n;&f_g;&a_w;&a_r;</ind></trd>
  <trd lng="fr">groseiller</trd>
  <trd lng="nl">kruisbes</trd>
  <trd lng="pl">agrest<klr> (krzew)</klr></trd>
  <trd lng="pt">groselheira</trd>
  <trd lng="ru">&c_k;&c_r;&c_y;&c_zh;&c_o;&c_v;&c_n;&c_i;&c_k;
  <klr>(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
</drv>
</art>
