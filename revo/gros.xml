<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gros.xml,v 1.28 2022/04/30 08:42:40 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>gros</rad>/o
</kap>

<drv mrk="gros.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gros.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Frukto de <tld/>ujo:
      <ekz>
        Maria pa&scirc;etadis inter la berarbedoj, gustumante ru&gcirc;ajn, blankajn kaj nigrajn
        ribojn kaj la plej dol&ccirc;ajn <tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Geraldo Mattos</aut>
          <lok><url ref="&Steloj;monato2/006918.html">&Steloj;monato2/006918.html</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="../bld/gros.jpg">bran&ccirc;o kun <tld/>oj</bld>
  </snc>
  <trd lng="be">&c_a;&c_g;&c_r;&c_e;&c_s;&c_t;<klr> (&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="bg">&c_c;&c_a;&c_r;&c_i;&c_g;&c_r;&c_a;&c_d;&c_s;&c_k;&c_o; &c_g;&c_r;&c_o;&c_z;&c_d;&c_je;</trd>
  <trd lng="ca">agrass&oacute;</trd>
  <trd lng="de">Stachelbeere</trd>
  <trd lng="en">gooseberry</trd>
  <trdgrp lng="fa">
    <trd>&f_g;&a_A;&a_l;&a_sh;&zwnj;&a_A;&a_n;&f_g;&a_w;&a_r;</trd>,
    <trd>&a_A;&a_n;&f_g;&a_w;&a_r;&a_f;&a_r;&a_n;&f_g;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">groseille<klr> (&agrave; maquereau)</klr></trd>
  <trdgrp lng="hu">
    <trd>egres</trd>,
    <trd>k&ouml;szm&eacute;te</trd>
  </trdgrp>
  <trd lng="pl">agrest</trd>
  <trd lng="pt">groselha</trd>
  <trd lng="ru">&c_k;&c_r;&c_y;&c_zh;&c_o;&c_v;&c_n;&c_i;&c_k; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="sk">egre&scaron;</trd>
  <trdgrp lng="cs">
    <trd>angre&scaron;t</trd>,
    <trd>srstka <klr>(ke&rcaron;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="gros.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="gros.0ujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:botanikaj_specioj">Specio</ref>
      de <ref tip="super" cel="rib.0o">ribo</ref>,
      kun grandaj acidaj, ofte haraj beroj,
      uzataj en konfita&jcirc;o ktp
      (<trd lng="la">Ribes uva-crispa</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_g;&c_r;&c_e;&c_s;&c_t; <klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trd lng="ca">agrass&oacute;</trd>
  <trdgrp lng="cs">
    <trd>angre&scaron;t</trd>,
    <trd>srstka <klr>(ke&rcaron;)</klr></trd>
  </trdgrp>
  <trd lng="de">Stachelbeerstrauch</trd>
  <trd lng="en">gooseberry</trd>
  <trd lng="fa">&a_d;&a_r;&a_kh;&a_t;&f_ch;&a_h;&a_hamza_sure; <ind>&f_g;&a_A;&a_l;&a_sh;&zwnj;&a_A;&a_n;&f_g;&a_w;&a_r;</ind></trd>
  <trd lng="fr">groseiller <klr>(&agrave; maquereau)</klr></trd>
  <trd lng="nl">kruisbes</trd>
  <trd lng="pl">agrest<klr> (krzew)</klr></trd>
  <trd lng="pt">groselheira</trd>
  <trd lng="ru">&c_k;&c_r;&c_y;&c_zh;&c_o;&c_v;&c_n;&c_i;&c_k; <klr>(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">egre&scaron;  <klr>(strom&ccaron;ek)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x430;&#x491;&#x440;&#x443;&#x441;</trd>,
    <trd>&#x43A;&#x443;&#x449;&#xA0;&#x430;&#x491;&#x440;&#x443;&#x441;&#x443;</trd>
  </trdgrp>
</drv>
</art>
