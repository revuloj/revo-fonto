<rad>kvadratur</rad>/o
</kap>
<drv mrk="kvadra2.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(de <ref cel="figur.geometria0o">geometria figuro</ref>)</klr>
