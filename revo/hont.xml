<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hont.xml,v 1.46 2026/01/20 08:50:06 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>hont</rad>/i <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="hont.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="hont.0i.PSI">
    <dif>
      Senti malagrablan konfuzi&gcirc;on kaj humili&gcirc;on, pro konscio je
      malhonora&jcirc;o, malhonesta&jcirc;o, maldeca&jcirc;o:
      <ekz>
        amba&ubreve; estis nudaj, <klr>[sed]</klr> ili ne <tld/>is
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 2:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel vi ne <tld/>as, granda&gcirc;a viro, tiel brui pro kelkaj mankantaj butonoj
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>!
      </ekz>
      <ekz>
        &ccirc;u vi ne <tld/>as? maljuna homo kaj kredas infanajn fabelojn
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>!
      </ekz>
      <ekz>
        mi ne <tld/>as konfesi, ke mi &gcirc;in rigardadis kun tiaj samaj profundaj sentoj,
        kiel vi
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi ne <tld/>as peti almozojn, vi estas juna kaj sana, vi povas labori
        <fnt>
          <bib>Marta</bib>
        </fnt>!
      </ekz>
      <ekz>
        mi <tld/>us, reaperante anta&ubreve; tiuj, kiuj min konis, kiam mi estis ri&ccirc;a
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekdua</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu <tld/>as nenion, ne timas Dion
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ne <tld/>u penti pri faro, <tld/>u persisti en eraro
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="gxen.sin0i">sin &gcirc;eni</ref>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_r;&c_o;&c_m;&c_je;&c_c;&c_c;&c_a;</trd>
  <trd lng="br">kaout <ind>mezh</ind></trd>
  <trdgrp lng="ca">
    <trd>avergonyir-se</trd>,
    <trd>apocar-se</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>styd&ecaron;t se</trd>,
    <trd>styd&ecaron;t se</trd>
  </trdgrp>
  <trd lng="de">sich <ind>sch&auml;men</ind></trd>
  <trd lng="el">&ny;&tau;&rho;&epsilon_ton;&pi;&omikron;&my;&alfa;&jota;</trd>
  <trd lng="en">be <ind>ashamed</ind></trd>
  <trd lng="es">avergonzarse de</trd>
  <trdgrp lng="fr">
    <trd>avoir <ind>honte</ind> (de)</trd>,
    <trd>rougir (de)<klr tip="ind"> (par honte)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&eacute;gyenkezik</trd>,
    <trd>sz&eacute;gyenli mag&aacute;t</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>canggung</trd>,
    <trd>malu</trd>,
    <trd><baz>rasa</baz>merasa malu</trd>,
    <trd>segan</trd>
  </trdgrp>
  <trd lng="it">vergognarsi</trd>
  <trdgrp lng="ja">
    <trd>&#x6065;&#x3058;&#x308B;  <pr>&#x306F;&#x3058;&#x308B;</pr></trd>,
    <trd>&#x6065;&#x305A;&#x304B;&#x3057;&#x304C;&#x308B;
    <pr>&#x306F;&#x305A;&#x304B;&#x3057;&#x304C;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">zich <ind>schamen</ind></trd>
  <trd lng="pl">wstydzi&cacute; si&eogonek;</trd>
  <trdgrp lng="pt">
   <trd>envergonhar-se de</trd>,
   <trd>pejar-se</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_y;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="sk">hanbi&tcaron; sa</trd>
  <trd lng="sv">sk&auml;mmas</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_o;&c_r;&c_o;&c_m;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_i;&c_d;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6C92;&#x81C9; <pr>m&eacute;ili&#x1CE;n</pr></trd>,
    <trd>&#x7F9E;&#x60ED; <pr>xi&umacron;c&aacute;n</pr></trd>,
    <trd>&#x77A2; <pr>m&eacute;ng</pr></trd>,
    <trd>&#x60ED;&#x6127; <pr>c&aacute;nku&igrave;</pr></trd>,
    <trd>&#x96BE;&#x4E3A;&#x60C5; <pr>n&aacute;nw&eacute;iq&iacute;ng</pr></trd>,
    <trd>&#x5FDD; <pr>ti&#x1CE;n</pr></trd>,
    <trd>&#x611F;&#x5230;&#x7F9E;&#x6127; <pr>g&#x1CE;nd&agrave;oxi&umacron;ku&igrave;</pr></trd>,
    <trd>&#x60ED; <pr>c&aacute;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hont.0o">
  <kap><tld/>o</kap>
  <snc mrk="hont.0o.PSI">
    <dif>
      <ref tip="lst" cel="sent.0o.emocio" lst="voko:sentoj">Sento</ref>
      de <tld/>anto:
      <ekz>
        li pali&gcirc;is de timo kaj poste li ru&gcirc;i&gcirc;is de <tld/>o
        <fnt>
          <bib>F</bib>
          <lok>&FE; 39</lok>
        </fnt>;
      </ekz>
      <ekz>
        li kulpigis min anta&ubreve; vi kaj ka&ubreve;zis al mi tian <tld/>on
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>!
      </ekz>
      <ekz>
        li levi&gcirc;is de la benko por iri rekte en la lo&gcirc;ejon de <nom>Kama</nom>, sed la <tld/>o
        haltigis lin
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        kie timo, tie <tld/>o<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>torbaci&oacute;</trd>,
      <trd>rubor</trd>
    </trdgrp>
    <trd lng="de">Scham</trd>
  <trd lng="id">rasa malu</trd>
  </snc>
  <snc mrk="hont.0o.hontindajxo">
    <dif>
      <ref tip="dif" cel="hont.0indajxo"><tld lit="H"/>inda&jcirc;o</ref>:
      <ekz>
        estus granda <tld/>o, se vi ne scius vian lecionon
        <fnt>
          <bib>Fab2</bib>
          <lok>Malgranda Tuk</lok>
        </fnt>!
      </ekz>
      <ekz>
        lia <tld/>o ne elvi&scirc;i&gcirc;as
        <fnt><bib>MT</bib>, <lok>&Sen; 6:33</lok></fnt>;
      </ekz>
      <ekz>
        fi, estas peko kaj <tld/>o, kia ordo estas &ccirc;i tie
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>!
      </ekz>
      <ekz>
        ili hakis sin reciproke &gcirc;is sango kaj falis senvive, ka&ubreve;zante malhonoron kaj
        <tld/>on al sia familio
        <fnt>
          <bib>Fab3</bib>
          <lok>Estas tute certe</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="makul.0o.FIG">makulo<sncref/></ref>,
      <ref cel="glor.mal0o">malgloro</ref>,
      <ref cel="honor.mal0o">malhonoro</ref>
    </refgrp>
    <trdgrp lng="ca">
      <trd>oprobi</trd>,
      <trd>inf&agrave;mia</trd>
    </trdgrp>
    <trd lng="de">Schande</trd>
  <trdgrp lng="id">
    <trd>aib</trd>,
    <trd>cela</trd>,
    <trd>noda</trd>
  </trdgrp>
  </snc>
  <trd lng="be">&c_s;&c_o;&c_r;&c_a;&c_m;</trd>
  <trd lng="br">mezh</trd>
  <trd lng="ca">vergonya</trd>
  <trdgrp lng="cs">
    <trd>hanba</trd>,
    <trd>stud</trd>
  </trdgrp>
  <trd lng="en">shame</trd>
  <trd lng="es">verg&uuml;enza</trd>
  <trd lng="fr">honte</trd>
  <trd lng="hu">sz&eacute;gyen</trd>
  <trd lng="it">vergogna</trd>
  <trdgrp lng="ja">
    <trd>&#x6065;  <pr>&#x306F;&#x3058;</pr></trd>,
    <trd>&#x6065;&#x8FB1;  <pr>&#x3061;&#x3058;&#x3087;&#x304F;</pr></trd>,
    <trd>&#x6065;&#x305A;&#x304B;&#x3057;&#x3055;  <pr>&#x306F;&#x305A;&#x304B;&#x3057;&#x3055;</pr></trd>,
    <trd>&#x7F9E;&#x6065;  <pr>&#x3057;&#x3085;&#x3046;&#x3061;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schaamte</trd>,
    <trd>schande</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wstyd</trd>
  </trdgrp>
  <trd lng="pt">vergonha</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_y;&c_d;</trd>,
    <trd>&c_s;&c_r;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hanba</trd>,
    <trd>hanblivos&tcaron;</trd>,
    <trd>stud</trd>
  </trdgrp>
  <trd lng="sv">skam</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_o;&c_r;&c_o;&c_m;</trd>,
    <trd>&c_s;&c_t;&c_i;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6065; <pr>ch&#x1D0;</pr></trd>,
    <trd>&#x6065;&#x8FB1; <pr>ch&#x1D0;r&#x1D4;</pr></trd>,
    <trd>&#x7F9E;&#x6065; <pr>xi&umacron;ch&#x1D0;</pr></trd>,
    <trd>&#x8A6C; <pr>g&ograve;u</pr></trd>,
    <trd>&#x8B11; <pr>x&igrave;</pr></trd>,
    <trd>&#x8D67; <pr>n&#x1CE;n</pr></trd>,
    <trd>&#x8FB1; <pr>r&#x1D4;</pr></trd>,
    <trd>&#x7F9E;&#x6127; <pr>xi&umacron;ku&igrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hont.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="hont.0ema.PSI">
    <dif>
      Facile kaj plej ofte senka&ubreve;ze <tld/>anta:
      <ekz>
        tiu timema kaj <tld/>ema knabino ru&gcirc;i&gcirc;as anta&ubreve;
        &ccirc;iu nekonata persono;
      </ekz>
      <ekz>
        &gcirc;i fari&gcirc;is timema kaj <tld/>ema kaj ka&scirc;is la kapon
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        jam kiel infano li estis izoli&gcirc;ema kaj <tld/>ema
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=319719">Robert
          Musil</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        vi ne havas e&ccirc; tiom da <tld/>emeco, por <tld/>i pro tiuj petoloj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>?
      </ekz>
    </dif>
    <ref tip="vid" cel="embara.0a.embarasata">embarasa<sncref/></ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_r;&c_a;&c_m;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_s;&c_a;&c_r;&c_a;&c_m;&c_ja;&c_zh;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_s;&c_a;&c_r;&c_o;&c_m;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>mezhek</trd>,
    <trd>lent</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>vergony&oacute;s</trd>,
    <trd>t&iacute;mid</trd>
  </trdgrp>
  <trd lng="cs">stydliv&yacute;</trd>
  <trdgrp lng="de">
    <trd>schamhaft</trd>,
    <trd>versch&auml;mt</trd>,
    <trd>sch&uuml;chtern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bashful</trd>,
    <trd>modest</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>vergonzoso</trd>,
    <trd>t&iacute;mido</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>honteux<klr tip="ind"> (rougissant)</klr></trd>,
    <trd>timide</trd>,
    <trd>rougissant</trd>
  </trdgrp>
  <trd lng="hu">sz&eacute;gyenl&odblac;s</trd>
  <trdgrp lng="id">
    <trd><baz>malu</baz>pemalu</trd>,
    <trd>rendah diri</trd>,
    <trd>minder</trd>
  </trdgrp>
  <trd lng="it">pudico</trd>
  <trdgrp lng="ja">
    <trd>&#x6065;&#x305A;&#x304B;&#x3057;&#x304C;&#x308A;&#x306E;
    <pr>&#x306F;&#x305A;&#x304B;&#x3057;&#x304C;&#x308A;&#x306E;</pr></trd>,
    <trd>&#x306F;&#x306B;&#x304B;&#x307F;&#x5C4B;&#x306E;
    <pr>&#x306F;&#x306B;&#x304B;&#x307F;&#x3084;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">beschaamd</trd>
  <trd lng="pl">wstydliwy</trd>
  <trd lng="pt">envergonhado</trd>
  <trd lng="ru">&c_s;&c_t;&c_y;&c_d;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
  <trd lng="sk">hanbliv&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_o;&c_r;&c_o;&c_m;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_o;&c_r;&c_o;&c_m;&#x2019;&c_ja;&c_z;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_o;&c_r;&c_o;&c_m;&#x2019;&c_ja;&c_z;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x817C;&#x8146; <pr>mi&#x1CE;nti&#x1CE;n</pr></trd>,
    <trd>&#x6110; <pr>mi&#x1CE;n</pr></trd>,
    <trd>&#x5FF8;&#x6029; <pr>ni&#x1D4;n&iacute;</pr></trd>,
    <trd>&#x6029; <pr>n&iacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hont.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="hont.0igi.PSI">
    <dif>
      Fari, ke iu <tld/>u:
      <ekz>
        <tld/>igi mallaboreman lernanton;
      </ekz>
      <ekz>
        kunulo de mal&ccirc;astuloj <tld/>igas sian patron
        <fnt><bib>MT</bib>, <lok>&Sen; 28:7</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la membro de unu nacio humili&gcirc;as anta&ubreve; la membro de alia nacio, parolas lian
        lingvon, <tld/>igante la sian
        <fnt>
          <bib>VivZam</bib>
          <lok>Vivo de Zamenhof</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="embara.0i.FIG">embarasi<sncref/></ref>,
      <ref cel="gxen.0i.morale">&gcirc;eni<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_r;&c_o;&c_m;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_a;&c_r;&c_a;&c_m;&c_a;&c_c;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_a;&c_r;&c_a;&c_m;&c_ja;&c_zh;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">mezhekaat<klr tip="ind"> (v.k.)</klr></trd>
  <trd lng="ca">avergonyir</trd>
  <trd lng="cs">zahanbit</trd>
  <trdgrp lng="de">
    <trd>besch&auml;men</trd>,
    <trd>blamieren</trd>
  </trdgrp>
  <trd lng="en">shame</trd>
  <trd lng="es">avergonzar</trd>
  <trd lng="fr">faire <ind>honte</ind> (&agrave;)</trd>
  <trd lng="hu">megsz&eacute;gyen&iacute;t</trd>
  <trdgrp lng="id">
    <trd><baz>buat</baz>membuat malu</trd>,
    <trd><baz>malu</baz>memalukan</trd>
  </trdgrp>
  <trd lng="it">svergognare</trd>
  <trdgrp lng="ja">
    <trd>&#x6065;&#x3092;&#x304B;&#x304B;&#x305B;&#x308B;
    <pr>&#x306F;&#x3058;&#x3092;&#x304B;&#x304B;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x6065;&#x305A;&#x304B;&#x3057;&#x304C;&#x3089;&#x305B;&#x308B;
    <pr>&#x306F;&#x305A;&#x304B;&#x3057;&#x304C;&#x3089;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">beschamen</trd>
  <trd lng="pl">zawstydza&cacute;</trd>
  <trd lng="pt">envergonhar</trd>
  <trd lng="ru">&c_s;&c_t;&c_y;&c_d;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">zahanbi&tcaron;</trd>
  <trd lng="uk">&c_s;&c_o;&c_r;&c_o;&c_m;&c_i;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x4F7F;&#x611F;&#x5230;&#x7F9E;&#x6127; <pr>sh&#x1D0;g&#x1CE;nd&agrave;oxi&umacron;ku&igrave;</pr></trd>,
    <trd>&#x52BF;&#x611F;&#x5230;&#x53EF;&#x803B;
    <pr>sh&igrave;g&#x1CE;nd&agrave;ok&ecaron;ch&#x1D0;</pr></trd>,
    <trd>&#x53EF;&#x6065; <pr>k&ecaron;ch&#x1D0;</pr></trd>,
    <trd>&#x5E94;&#x53D7;&#x8C34;&#x8D23; <pr>y&igrave;ngsh&ograve;uqi&#x1CE;nz&eacute;</pr></trd>,
    <trd>&#x4F7F;&#x56F0;&#x7A98; <pr>sh&#x1D0;k&ugrave;nji&#x1D2;ng</pr></trd>,
    <trd>&#x8FC7;&#x4E0D;&#x53BB; <pr>gu&ograve;buq&ugrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hont.0inda">
  <kap><tld/>inda</kap>
  <snc mrk="hont.0inda.PSI">
    <dif>
      Pri kiu oni devas <tld/>i:
      <ekz>
        <klr>[se]</klr> li trovis &ccirc;e &scirc;i ion <tld/>indan,
        tiam li skribu al &scirc;i eksedzigan leteron
        <fnt><bib>MT</bib>, <lok>&Rea; 24:1</lok></fnt>;
      </ekz>
      <ekz>
        kiu dormas en la tempo de rikolto,
        tiu estas filo <tld/>inda
        <fnt><bib>MT</bib>, <lok>&Sen; 10:5</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="embara.0a.embarasanta">embarasa<sncref/></ref>
  </snc>
  <trdgrp lng="be">
