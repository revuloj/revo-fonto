  <trd lng="fr">honte</trd>
  <trd lng="hu">sz&eacute;gyenletes dolog</trd>
  <trd lng="it">ignominia</trd>
  <trdgrp lng="ja">
    <trd>&#24677;&#12378;&#12409;&#12365;&#34892;&#28858;  <pr>&#12399;&#12378;&#12409;&#12365;&#12371;&#12358;&#12356;</pr></trd>,
    <trd>&#24677;&#30693;&#12425;&#12378;&#12394;&#35328;&#21205;  <pr>&#12399;&#12376;&#12375;&#12425;&#12378;&#12394;&#12370;&#12435;&#12393;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">schande</trd>
  <trd lng="es">verg&uuml;enza</trd>
  <trdgrp lng="id">
    <trd>aib</trd>,
    <trd>cela</trd>,
    <trd>noda</trd>
  </trdgrp>
  <trd lng="pt">vergonha</trd>
  <trd lng="pl">wstydliwa rzecz</trd>
  <trd lng="ru">&c_p;&c_o;&c_z;&c_o;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#24677; <pr>ch&#464;</pr></trd>,
    <trd>&#35601; <pr>x&igrave;</pr></trd>,
    <trd>&#24677;&#36785; <pr>ch&#464;r&#468;</pr></trd>,
    <trd>&#36785; <pr>r&#468;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hont.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="hont.pri0i.KOMUNE">
    <dif>
      <tld lit="H"/>i pro:
      <ekz>
