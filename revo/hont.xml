<vortaro>
<art mrk="$Id: hont.xml,v 1.45 2025/09/24 08:39:44 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>hont</rad>/i <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="hont.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="hont.0i.PSI">
    <dif>
      Senti malagrablan konfuzi&gcirc;on kaj humili&gcirc;on, pro konscio je
      malhonora&jcirc;o, malhonesta&jcirc;o, maldeca&jcirc;o:
      <ekz>
        vi devus <tld/>i vian teni&gcirc;on a&ubreve; pri, pro via
        teni&gcirc;o;
      </ekz>
      <ekz>
        kiu <tld/>as nenion, ne timas Dion
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        mi <tld/>as, ke mi tiel longe prokrastis mian viziton;
      </ekz>
      <ekz>
        ne <tld/>u penti pri faro, <tld/>u persisti en eraro
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la membro de unu nacio humili&gcirc;as anta&ubreve; la membro de alia
        nacio, parolas lian lingvon, <tld/>ante la sian
        <fnt><bib>Paroloj</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_r;&c_o;&c_m;&c_je;&c_c;&c_c;&c_a;</trd>
  <trd lng="br">kaout <ind>mezh</ind></trd>
  <trdgrp lng="ca">
    <trd>avergonyir-se</trd>,
    <trd>apocar-se</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>styd&ecaron;t se</trd>,
    <trd>styd&ecaron;t se</trd>
  </trdgrp>
  <trd lng="de">sich <ind>sch&auml;men</ind></trd>
  <trd lng="el">&ny;&tau;&rho;&epsilon_ton;&pi;&omikron;&my;&alfa;&jota;</trd>
  <trd lng="en">be <ind>ashamed</ind></trd>
  <trd lng="es">avergonzarse de</trd>
  <trdgrp lng="fr">
    <trd>avoir <ind>honte</ind> (de)</trd>,
    <trd>rougir (de)<klr tip="ind"> (par honte)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&eacute;gyenkezik</trd>,
    <trd>sz&eacute;gyenli mag&aacute;t</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>canggung</trd>,
    <trd>malu</trd>,
    <trd><baz>rasa</baz>merasa malu</trd>,
    <trd>segan</trd>
  </trdgrp>
  <trd lng="it">vergognarsi</trd>
  <trdgrp lng="ja">
    <trd>&#24677;&#12376;&#12427;  <pr>&#12399;&#12376;&#12427;</pr></trd>,
    <trd>&#24677;&#12378;&#12363;&#12375;&#12364;&#12427;
    <pr>&#12399;&#12378;&#12363;&#12375;&#12364;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">zich <ind>schamen</ind></trd>
  <trd lng="pl">wstydzi&cacute; si&eogonek;</trd>
  <trdgrp lng="pt">
   <trd>envergonhar-se de</trd>,
   <trd>pejar-se</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_y;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="sk">hanbi&tcaron; sa</trd>
  <trd lng="sv">sk&auml;mmas</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_o;&c_r;&c_o;&c_m;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_i;&c_d;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#27794;&#33225; <pr>m&eacute;ili&#462;n</pr></trd>,
    <trd>&#32670;&#24813; <pr>xi&umacron;c&aacute;n</pr></trd>,
    <trd>&#30626; <pr>m&eacute;ng</pr></trd>,
    <trd>&#24813;&#24871; <pr>c&aacute;nku&igrave;</pr></trd>,
    <trd>&#38590;&#20026;&#24773; <pr>n&aacute;nw&eacute;iq&iacute;ng</pr></trd>,
    <trd>&#24541; <pr>ti&#462;n</pr></trd>,
    <trd>&#24863;&#21040;&#32670;&#24871; <pr>g&#462;nd&agrave;oxi&umacron;ku&igrave;</pr></trd>,
    <trd>&#24813; <pr>c&aacute;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hont.0o">
  <kap><tld/>o</kap>
  <snc mrk="hont.0o.PSI">
    <dif>
      <ref tip="lst" cel="sent.0o.emocio" lst="voko:sentoj">Sento</ref>
      de <tld/>anto:
      <ekz>
        ru&gcirc;i&gcirc;i pro <tld/>o<fnt>B</fnt>;
      </ekz>
      <ekz>
        kie timo, tie <tld/>o<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        fari <tld/>on al iu.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>torbaci&oacute;</trd>,
      <trd>rubor</trd>
    </trdgrp>
    <trd lng="de">Scham</trd>
  <trd lng="id">rasa malu</trd>
  </snc>
  <snc mrk="hont.0o.hontindajxo">
      <dif>
        <ref tip="dif" cel="hont.0indajxo"><tld lit="H"/>inda&jcirc;o</ref>:
        <ekz>
          lia <tld/>o ne elvi&scirc;i&gcirc;as
          <fnt><bib>MT</bib>, <lok>&Sen; 6:33</lok></fnt>;
        </ekz>
        <ekz>
          li estas la <tld/>o de sia familio;
        </ekz>
        <ekz>
          mi ne transvivos tiun <tld/>on;
        </ekz>
        <ekz>
          tia malpureco estas ja <tld/>o.
        </ekz>
      </dif>
    <refgrp tip="vid">
      <ref cel="makul.0o">makulo</ref>,
      <ref cel="glor.mal0o">malgloro</ref>.
    </refgrp>
    <trdgrp lng="ca">
      <trd>oprobi</trd>,
      <trd>inf&agrave;mia</trd>
    </trdgrp>
    <trd lng="de">Schande</trd>
  <trdgrp lng="id">
    <trd>aib</trd>,
    <trd>cela</trd>,
    <trd>noda</trd>
  </trdgrp>
  </snc>
  <trd lng="be">&c_s;&c_o;&c_r;&c_a;&c_m;</trd>
  <trd lng="br">mezh</trd>
  <trd lng="ca">vergonya</trd>
  <trdgrp lng="cs">
    <trd>hanba</trd>,
    <trd>stud</trd>
  </trdgrp>
  <trd lng="en">shame</trd>
  <trd lng="es">verg&uuml;enza</trd>
  <trd lng="fr">honte</trd>
  <trd lng="hu">sz&eacute;gyen</trd>
  <trd lng="it">vergogna</trd>
  <trdgrp lng="ja">
    <trd>&#24677;  <pr>&#12399;&#12376;</pr></trd>,
    <trd>&#24677;&#36785;  <pr>&#12385;&#12376;&#12423;&#12367;</pr></trd>,
    <trd>&#24677;&#12378;&#12363;&#12375;&#12373;  <pr>&#12399;&#12378;&#12363;&#12375;&#12373;</pr></trd>,
    <trd>&#32670;&#24677;  <pr>&#12375;&#12421;&#12358;&#12385;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schaamte</trd>,
    <trd>schande</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wstyd</trd>
  </trdgrp>
  <trd lng="pt">vergonha</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_y;&c_d;</trd>,
    <trd>&c_s;&c_r;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hanba</trd>,
    <trd>hanblivos&tcaron;</trd>,
    <trd>stud</trd>
  </trdgrp>
  <trd lng="sv">skam</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_o;&c_r;&c_o;&c_m;</trd>,
    <trd>&c_s;&c_t;&c_i;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#24677; <pr>ch&#464;</pr></trd>,
    <trd>&#24677;&#36785; <pr>ch&#464;r&#468;</pr></trd>,
    <trd>&#32670;&#24677; <pr>xi&umacron;ch&#464;</pr></trd>,
    <trd>&#35436; <pr>g&ograve;u</pr></trd>,
    <trd>&#35601; <pr>x&igrave;</pr></trd>,
    <trd>&#36199; <pr>n&#462;n</pr></trd>,
    <trd>&#36785; <pr>r&#468;</pr></trd>,
    <trd>&#32670;&#24871; <pr>xi&umacron;ku&igrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hont.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="hont.0ema.PSI">
    <dif>
      Facile kaj plej ofte senka&ubreve;ze <tld/>anta:
      <ekz>
        &gcirc;i fari&gcirc;is timema kaj <tld/>ema kaj ka&scirc;is la kapon
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        jam kiel infano li estis izoli&gcirc;ema kaj <tld/>ema
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=319719">Robert
          Musil</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu timema kaj <tld/>ema knabino ru&gcirc;i&gcirc;as anta&ubreve;
        &ccirc;iu nekonata persono.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_r;&c_a;&c_m;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_s;&c_a;&c_r;&c_a;&c_m;&c_ja;&c_zh;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_s;&c_a;&c_r;&c_o;&c_m;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>mezhek</trd>,
    <trd>lent</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>vergony&oacute;s</trd>,
    <trd>t&iacute;mid</trd>
  </trdgrp>
  <trd lng="cs">stydliv&yacute;</trd>
  <trdgrp lng="de">
    <trd>schamhaft</trd>,
    <trd>versch&auml;mt</trd>,
    <trd>sch&uuml;chtern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bashful</trd>,
    <trd>modest</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>vergonzoso</trd>,
    <trd>t&iacute;mido</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>honteux<klr tip="ind"> (rougissant)</klr></trd>,
    <trd>timide</trd>,
    <trd>rougissant</trd>
  </trdgrp>
  <trd lng="hu">sz&eacute;gyenl&odblac;s</trd>
  <trdgrp lng="id">
    <trd><baz>malu</baz>pemalu</trd>,
    <trd>rendah diri</trd>,
    <trd>minder</trd>
  </trdgrp>
  <trd lng="it">pudico</trd>
  <trdgrp lng="ja">
    <trd>&#24677;&#12378;&#12363;&#12375;&#12364;&#12426;&#12398;
    <pr>&#12399;&#12378;&#12363;&#12375;&#12364;&#12426;&#12398;</pr></trd>,
    <trd>&#12399;&#12395;&#12363;&#12415;&#23627;&#12398;
    <pr>&#12399;&#12395;&#12363;&#12415;&#12420;&#12398;</pr></trd>
  </trdgrp>
  <trd lng="nl">beschaamd</trd>
  <trd lng="pl">wstydliwy</trd>
  <trd lng="pt">envergonhado</trd>
  <trd lng="ru">&c_s;&c_t;&c_y;&c_d;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
  <trd lng="sk">hanbliv&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_o;&c_r;&c_o;&c_m;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_o;&c_r;&c_o;&c_m;&#8217;&c_ja;&c_z;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_o;&c_r;&c_o;&c_m;&#8217;&c_ja;&c_z;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#33148;&#33094; <pr>mi&#462;nti&#462;n</pr></trd>,
    <trd>&#24848; <pr>mi&#462;n</pr></trd>,
    <trd>&#24568;&#24617; <pr>ni&#468;n&iacute;</pr></trd>,
    <trd>&#24617; <pr>n&iacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hont.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="hont.0igi.PSI">
    <dif>
      Fari, ke iu <tld/>u:
      <ekz>
        <tld/>igi mallaboreman lernanton;
      </ekz>
      <ekz>
        kunulo de mal&ccirc;astuloj <tld/>igas sian patron
        <fnt><bib>MT</bib>, <lok>&Sen; 28:7</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_r;&c_o;&c_m;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_a;&c_r;&c_a;&c_m;&c_a;&c_c;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_a;&c_r;&c_a;&c_m;&c_ja;&c_zh;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">mezhekaat<klr tip="ind"> (v.k.)</klr></trd>
  <trd lng="ca">avergonyir</trd>
  <trd lng="cs">zahanbit</trd>
  <trd lng="de">besch&auml;men</trd>
  <trd lng="en">shame</trd>
  <trd lng="es">avergonzar</trd>
  <trd lng="fr">faire <ind>honte</ind> (&agrave;)</trd>
  <trd lng="hu">megsz&eacute;gyen&iacute;t</trd>
  <trdgrp lng="id">
    <trd><baz>buat</baz>membuat malu</trd>,
    <trd><baz>malu</baz>memalukan</trd>
  </trdgrp>
  <trd lng="it">svergognare</trd>
  <trdgrp lng="ja">
    <trd>&#24677;&#12434;&#12363;&#12363;&#12379;&#12427;
    <pr>&#12399;&#12376;&#12434;&#12363;&#12363;&#12379;&#12427;</pr></trd>,
    <trd>&#24677;&#12378;&#12363;&#12375;&#12364;&#12425;&#12379;&#12427;
    <pr>&#12399;&#12378;&#12363;&#12375;&#12364;&#12425;&#12379;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">beschamen</trd>
  <trd lng="pl">zawstydza&cacute;</trd>
  <trd lng="pt">envergonhar</trd>
  <trd lng="ru">&c_s;&c_t;&c_y;&c_d;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">zahanbi&tcaron;</trd>
  <trd lng="uk">&c_s;&c_o;&c_r;&c_o;&c_m;&c_i;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#20351;&#24863;&#21040;&#32670;&#24871; <pr>sh&#464;g&#462;nd&agrave;oxi&umacron;ku&igrave;</pr></trd>,
    <trd>&#21183;&#24863;&#21040;&#21487;&#32827;
    <pr>sh&igrave;g&#462;nd&agrave;ok&ecaron;ch&#464;</pr></trd>,
    <trd>&#21487;&#24677; <pr>k&ecaron;ch&#464;</pr></trd>,
    <trd>&#24212;&#21463;&#35892;&#36131; <pr>y&igrave;ngsh&ograve;uqi&#462;nz&eacute;</pr></trd>,
    <trd>&#20351;&#22256;&#31384; <pr>sh&#464;k&ugrave;nji&#466;ng</pr></trd>,
    <trd>&#36807;&#19981;&#21435; <pr>gu&ograve;buq&ugrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hont.0inda">
  <kap><tld/>inda</kap>
  <snc mrk="hont.0inda.PSI">
    <dif>
      Pri kiu oni devas <tld/>i:
      <ekz>
        <klr>[se]</klr> li trovis &ccirc;e &scirc;i ion <tld/>indan,
        tiam li skribu al &scirc;i eksedzigan leteron
        <fnt><bib>MT</bib>, <lok>&Rea; 24:1</lok></fnt>;
      </ekz>
      <ekz>
        kiu dormas en la tempo de rikolto,
        tiu estas filo <tld/>inda
        <fnt><bib>MT</bib>, <lok>&Sen; 10:5</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_n;&c_je;&c_b;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_a;&c_ch;&c_e;&c_s;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="br">mezhus</trd>
  <trdgrp lng="cs">
    <trd>hanebn&yacute;</trd>,
    <trd>inf&aacute;mn&iacute;</trd>,
    <trd>nestoudn&yacute;</trd>,
    <trd>nestydat&yacute;</trd>
  </trdgrp>
  <trd lng="en">shameful</trd>
  <trd lng="es">vergonzoso</trd>
  <trd lng="fr">honteux<klr tip="ind"> (qui fait
  honte)</klr></trd>
  <trd lng="id"><baz>malu</baz>memalukan<klr tip="amb"> (adj)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#24677;&#12378;&#12409;&#12365;  <pr>&#12399;&#12378;&#12409;&#12365;</pr></trd>,
    <trd>&#24677;&#12378;&#12363;&#12375;&#12356;  <pr>&#12399;&#12378;&#12363;&#12375;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="sk">hanebn&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_a;&c_n;&c_je;&c_b;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_g;&c_a;&c_n;&c_je;&c_b;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="hont.0indajxo">
  <kap><tld/>inda&jcirc;o</kap>
