<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: almoz.xml,v 1.34 2021/05/28 12:11:18 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>almoz</rad>/o
</kap>
<drv mrk="almoz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="almoz.0o.KOMUNE">
    <dif>
      Mondonaco, monhelpo al malri&ccirc;ulo:
      <ekz>petadi <tld/>on<fnt>Z</fnt>.</ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ib;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_n;&c_a;</trd>, 
    <trd>&c_m;&c_ib;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd> 
  </trdgrp>
  <trd lng="br">aluzen</trd>
  <trdgrp lng="cs">
    <trd>almu&zcaron;na</trd>,
    <trd>milodar</trd>
  </trdgrp>
  <trd lng="de">Almosen</trd>
  <trdgrp lng="en">
    <trd>alms</trd>,
    <trd>charity</trd>,
    <trd>charitable relief</trd>
  </trdgrp>
  <trd lng="es">limosna</trd>
  <trd lng="fr">aum&ocirc;ne</trd>
  <trd lng="he">&#x5E0;&#x5B0;&#x5D3;&#x5B8;&#x5D1;&#x5B8;&#x5D4;</trd>
  <trdgrp lng="id">
    <trd>amal</trd>,
    <trd>derma</trd>,
    <trd>sedekah</trd>
  </trdgrp>
  <trd lng="nl">aalmoes</trd>
  <trd lng="pl">ja&lstroke;mu&zdot;na</trd>
  <trd lng="pt">esmola</trd>
  <trd lng="ro">poman&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_i;&c_l;&c_o;&c_s;&c_t;&c_y;&c_n;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_a;&c_ja;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">almu&zcaron;na</trd>
  <trd lng="sv">allmosa</trd>
</drv>

<drv mrk="almoz.0i">
  <kap><tld/>i, <var><kap><tld/>peti<fnt>Z</fnt></kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="almoz.0i.almozpeti">
    <dif>
      Peti <tld/>e:
      <ekz>kiu tro ripozas, balda&ubreve; <tld/>as<fnt>Z</fnt>.</ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_a;&c_b;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>, 
    <trd>&c_h;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol; 
      &c_p;&c_a; &c_m;&c_ib;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>, 
    <trd>&c_p;&c_r;&c_a;&c_s;&c_ib;&c_c;&c_mol; 
      &c_m;&c_ib;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_n;&c_u;</trd> 
  </trdgrp>
  <trd lng="br">goulenn an <ind>aluzen</ind></trd>
  <trd lng="cs">&zcaron;ebrat <klr>(o n&ecaron;co)</klr></trd>
  <trd lng="de">betteln</trd>
  <trd lng="en">beg</trd>
  <trdgrp lng="es">
    <trd>limosnar</trd>,
    <trd>pedir limosna</trd>,
    <trd>mendigar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>demander l'<ind>aum&ocirc;ne</ind></trd>,
    <trd>mendier</trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5E7;&#x5D1;&#x5E5; &#x5E0;&#x5D3;&#x5D1;&#x5D5;&#x5EA;</trd>
  <trdgrp lng="id">
    <trd><baz>kemis</baz>mengemis</trd>,
    <trd>me<ind>minta</ind></trd>
  </trdgrp>
  <trd lng="nl">bedelen</trd>
  <trd lng="pl">&zdot;ebra&cacute;</trd>
  <trdgrp lng="pt">
    <trd>esmolar</trd>,
    <trd>pedir esmolar</trd>,
    <trd>mendigar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_i;&c_shch;&c_je;&c_n;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_p;&c_r;&c_o;&c_sh;&c_a;&c_j;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">&zcaron;obra&tcaron;</trd>
  <trd lng="sv">tigga</trd>
</drv>

<drv mrk="almoz.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="almoz.0ulo.KOMUNE">
    <dif>
      Homo, kiu petas pri <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_b;&c_r;&c_a;&c_k;</trd>
  <trd lng="bo">&#xF66;&#xFA4;&#xFB2;&#xF44;&#xF0B;&#xF54;&#xF7C;&#xF0B;</trd>
  <trdgrp lng="br">
    <trd>klasker-bara</trd>,
    <trd>klasker-boued</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>almu&zcaron;n&iacute;k</trd>,
    <trd>&zcaron;ebr&aacute;k</trd>
  </trdgrp>
  <trd lng="de">Bettler</trd>
  <trd lng="en">beggar</trd>
  <trdgrp lng="es">
    <trd>limosnero</trd>,
    <trd>pordiosero</trd>,
    <trd>mendigo</trd>
  </trdgrp>
  <trd lng="fr">mendiant</trd>
  <trd lng="he">&#x5E7;&#x5B7;&#x5D1;&#x5BC;&#x5B0;&#x5E6;&#x5B8;&#x5DF;</trd>
  <trdgrp lng="id">
    <trd><baz>kemis</baz> pengemis</trd>,
    <trd>pe<ind>minta</ind>-minta</trd>
  </trdgrp>
  <trd lng="nl">bedelaar</trd>
  <trd lng="pl">&zdot;ebrak</trd>
  <trdgrp lng="pt">
    <trd>mendigo</trd>,
    <trd>pedinte</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_i;&c_shch;&c_i;&c_j;</trd>
  <trd lng="sk">&zcaron;obr&aacute;k</trd>
  <trd lng="sv">tiggare</trd>
</drv>

<drv mrk="almoz.0ulejo">
  <kap><tld/>ulejo</kap>
  <snc mrk="almoz.0ulejo.KOMUNE">
    <dif>
      Rifu&gcirc;domo por <tld/>uloj.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_t;&c_u;&c_l;&c_a;&c_k; 
    <klr>(&c_zh;&c_a;&c_b;&c_r;&c_a;&c_c;&c_k;&c_ib;)</klr></trd>
  <trd lng="cs">chudobinec</trd>
  <trdgrp lng="de">
    <trd>Armenhaus</trd>,
    <trd>Armenheim</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>almshouse</trd>,
    <trd>poorhouse</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>asilo</trd>,
    <trd>hospicio</trd>,
    <trd>albergue para personas sin hogar</trd>,
    <trd>refugio para pobres</trd>
  </trdgrp>
  <trd lng="he">&#x5D1;&#x5D9;&#x5EA; &#x5E6;&#x5D3;&#x5E7;&#x5D4;</trd>
  <trd lng="pl">przytu&lstroke;ek</trd>
  <trd lng="pt">asilo <klr>de mendigos</klr></trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_ju;&c_t; 
    <klr>(&c_d;&c_l;&c_ja; &c_n;&c_i;&c_shch;&c_i;&c_h;)</klr></trd>
  <trd lng="sk">chudobinec</trd>
</drv>

<drv mrk="almoz.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="almoz.0isto.KOMUNE">
    <dif>
      Homo, kiu elvivas per <tld/>oj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_a;&c_b;&c_r;&c_a;&c_k;</trd>, 
    <trd>&c_p;&c_a;&c_p;&c_r;&c_a;&c_s;&c_ib;&c_m;&c_je;&c_c;</trd>, 
    <trd>&c_p;&c_a;&c_b;&c_ib;&c_r;&c_a;&c_h;&c_a;</trd> 
  </trdgrp>
  <trdgrp lng="br">
    <trd>klasker-bara</trd>,
    <trd>klasker-boued</trd>
  </trdgrp>
  <trd lng="de">Bettler</trd>
  <trdgrp lng="en">
    <trd>beggar</trd>,
    <trd>mendicant</trd>,
    <trd>almsman</trd>
  </trdgrp>
 <trd lng="he">&#x5E7;&#x5B7;&#x5D1;&#x5BC;&#x5B0;&#x5E6;&#x5B8;&#x5DF;</trd>
  <trdgrp lng="id">
    <trd><baz>kemis</baz> pengemis</trd>,
    <trd>pe<ind>minta</ind>-minta</trd>
  </trdgrp>
  <trd lng="nl">bedelaar</trd>
  <trd lng="pt">mendigo</trd>
  <trdgrp lng="es">
    <trd>limosnero</trd>,
    <trd>pordiosero</trd>,
    <trd>mendigo</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_i;&c_shch;&c_i;&c_j;</trd>
  <trd lng="sk">&zcaron;obr&aacute;k</trd>
  <trd lng="sv">tiggare</trd>
</drv>

<drv mrk="almoz.0kesto">
  <kap><tld/>kesto</kap>
  <snc mrk="almoz.0kesto.KOMUNE">
    <dif>
      Kesto en pre&gcirc;ejo kaj samspecaj lokoj, destinita ricevadi
      <tld/>ojn.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_r;&c_y;&c_n;&c_k;&c_a; <klr>(&c_n;&c_a; 
    &c_a;&c_h;&c_v;&c_ja;&c_r;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ib;)</klr>
  </trd>
  <trd lng="br">kef<klr tip="ind"> (da zastum an aluzen)</klr></trd>
  <trd lng="cs">schr&aacute;nka na dary <klr>(v kostele)</klr></trd>
  <trdgrp lng="de">
    <trd>Opferstock</trd>,
    <trd>Spendenb&uuml;chse</trd>
  </trdgrp>
  <trd lng="en">poor box</trd>
  <trd lng="es">cesta de limosnas</trd>
  <trd lng="fr">tronc</trd>
  <trd lng="he">&#x5EA;&#x5D9;&#x5D1;&#x5EA; &#x5E0;&#x5D3;&#x5D1;&#x5D4;</trd>
  <trd lng="id"><ind>kotak</ind> amal</trd>
  <trd lng="nl">offerblok</trd>
  <trd lng="pt">caixa de esmolas</trd>
  <trd lng="sk">schr&aacute;nka na dary</trd>
</drv>

<drv mrk="almoz.0sako">
  <kap><tld/>sako</kap>
  <snc mrk="almoz.0sako.KOMUNE">
    <dif>
      Sako, en kiun <tld/>ulo metas la ricevita&jcirc;ojn.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_o;&c_r;&c_b;&c_a; 
    <klr>(&c_zh;&c_a;&c_b;&c_r;&c_a;&c_c;&c_k;&c_a;&c_ja;)</klr></trd>
  <trd lng="cs">&zcaron;ebr&aacute;ck&aacute; mo&scaron;na</trd>
  <trdgrp lng="de">
    <trd>Bettelsack</trd>,
  </trdgrp>
  <trd lng="es">bolsa de lismonas</trd>
  <trd lng="nl">bedelzak</trd>
  <trd lng="pt">esmoleira</trd>
  <trd lng="ru">&c_s;&c_u;&c_m;&c_a; 
    <klr>(&c_n;&c_i;&c_shch;&c_je;&c_n;&c_s;&c_k;&c_a;&c_ja;)</klr></trd>
  <trd lng="sk">&zcaron;obr&aacute;cka ta&scaron;ka</trd>
</drv>

<drv mrk="almoz.0doni">
  <kap><tld/>doni</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Doni <tld/>on:
      <ekz>sur la &scirc;ultroj de la <tld/>donantoj<fnt>Z</fnt>.</ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; 
      <klr>(&c_m;&c_ib;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_n;&c_u;)</klr></trd>,
      
    <trd>&c_a;&c_h;&c_v;&c_ja;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
      
    
  </trdgrp>
  <trd lng="br">rei&ntilde; an <ind>aluzen</ind></trd>
  <trdgrp lng="cs">
    <trd>d&aacute;t almu&zcaron;nu</trd>,
    <trd>d&aacute;vat almu&zcaron;nu</trd>
  </trdgrp>
  <trd lng="en">give alms</trd>
  <trd lng="es">dar <ind>limosna</ind></trd>
  <trd lng="fr">faire l'aum&ocirc;ne</trd>
  <trdgrp lng="id">
    <trd>ber<ind>amal</ind></trd>,
    <trd>be<ind>derma</ind></trd>,
    <trd>ber<ind>sedekah</ind></trd>
  </trdgrp>
  <trd lng="pt">dar <ind>esmolas</ind></trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol; 
    <klr>(&c_n;&c_i;&c_shch;&c_i;&c_m;)</klr></trd>
  <trd lng="sk">da&tcaron; almu&zcaron;nu</trd>
</drv>

<drv mrk="almoz.0donema">
  <kap><tld/>donema</kap>
  <snc>
  <uzo tip="fak">EKON</uzo>
    <dif>
      Volonte kaj malavare donanta <tld/>ojn.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_v;&c_y;</trd>
  <trd lng="br">aluzenus</trd>
  <trdgrp lng="cs">
    <trd>naklon&ecaron;n d&aacute;vat almu&zcaron;nu</trd>,
    <trd>&scaron;t&ecaron;dr&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wohlt&auml;tig</trd>,
    <trd>karitativ</trd>
  </trdgrp>
  <trd lng="en">charitable</trd>
  <trd lng="es">caritativo</trd>
  <trd lng="fr">charitable</trd>
  <trd lng="he">&#x5E0;&#x5D3;&#x5D1;&#x5D4;</trd>
  <trd lng="id">dermawan</trd>
  <trd lng="pt">caridoso</trd>
  <trd lng="sk">&scaron;tedr&yacute;</trd>
</drv>
</art>
