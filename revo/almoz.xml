<drv mrk="almoz.0i">
  <kap><tld/>i, <var><kap><tld/>elpeti</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="almoz.0i.almozpeti">
    <dif>
      <ref tip="super" cel="pet.0i">Peti</ref> <tld/>e:
      <ekz>
        la infanoj estas devigataj <tld/>i
        <fnt>
        <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2003/008043.html">Seksa sklaveco: &ccirc;u defio al la 21a
          jarcento?</url></vrk>
          <bib>Monato</bib>
          <lok>2003/09, p. 20</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>as kiel batita hundo
        <fnt>
          <aut>Anna L&ouml;wenstein</aut>
          <vrk>La &scirc;tona urbo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi eltiros la manon... mi <tld/>elpetos... murmuretis al si Marta
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu tro ripozas, balda&ubreve; <tld/>as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        li <tld/>e elpetadis al si panon anta&ubreve; la pordoj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#21270;&#x7DE3; <pr>hu&#xE0;yu&#xE1;n</pr></trd>,
    <trd>&#21270;&#x7F18; <pr>hu&#xE0;yu&#xE1;n</pr></trd>,
    <trd>&#x544A;&#39250; <pr>g&#xE0;or&#xE1;o</pr></trd>,
    <trd>&#x544A;&#39286; <pr>g&#xE0;or&#xE1;o</pr></trd>,
    <trd>&#x4E5E;&#x6C42; <pr>q&#x1D0;qi&#xFA;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_a;&c_b;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_h;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;
      &c_p;&c_a; &c_m;&c_ib;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_s;&c_ib;&c_c;&c_mol;
      &c_m;&c_ib;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_n;&c_u;</trd>
  </trdgrp>
  <trd lng="br">goulenn an <ind>aluzen</ind></trd>
  <trd lng="cs">&zcaron;ebrat <klr>(o n&ecaron;co)</klr></trd>
  <trd lng="de">betteln</trd>
  <trd lng="en">beg</trd>
  <trdgrp lng="es">
    <trd>limosnar</trd>,
    <trd>pedir limosna</trd>,
    <trd>mendigar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>demander l'<ind>aum&ocirc;ne</ind></trd>,
    <trd>mendier</trd>
  </trdgrp>
  <trd lng="he">&lamed;&qof;&bet;&ftsadi; &nun;&dalet;&bet;&vav;&tav;</trd>
  <trdgrp lng="id">
    <trd><baz>kemis</baz>mengemis</trd>,
    <trd>me<ind>minta</ind></trd>
  </trdgrp>
  <trd lng="nl">bedelen</trd>
  <trd lng="pl">&zdot;ebra&cacute;</trd>
  <trdgrp lng="pt">
    <trd>esmolar</trd>,
    <trd>pedir esmolar</trd>,
    <trd>mendigar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_i;&c_shch;&c_je;&c_n;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_p;&c_r;&c_o;&c_sh;&c_a;&c_j;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">&zcaron;obra&tcaron;</trd>
  <trd lng="sv">tigga</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&#x43B;&c_i;&c_d;&c_a;&c_r;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_zh;&c_je;&c_b;&c_r;&c_a;&c_ch;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_r;&c_c;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="almoz.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="almoz.0ulo.KOMUNE">
    <dif>
      Petanto pri <tld/>o:
      <ekz>
        mi renkontis sur la vojo tri <tld/>ulojn kaj donis al &ccirc;iu kvaronon da bulo kaj
        unu gro&scirc;on
        <fnt>
          <bib>ElektFab</bib>
          <lok>Kamarado Gaja</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi, <tld/>ulo, estas e&ccirc; por danko tro malri&ccirc;a
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Tolstoj</nom> haltis kaj donis el sia monujo du ar&gcirc;entajn monerojn <klr>[&#8230;]</klr> al
        la <tld/>ulo, kiu surprizita pro la malavara donaco plenigis lian manon per
        kisoj
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
        </fnt>;
      </ekz>
      <ekz>
        kvankam li estas preska&ubreve; <tld/>ulo, sed li alportas al la urbo re&gcirc;an donacon: li
        proponas al &gcirc;i sian projekton
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        almena&ubreve; trovas bonegan pretekston por forpeli el mia domo tiun almozulinon; &ccirc;ar
        mi &ccirc;iam timis, ke mi estos devigata nomi &scirc;in iam: mia filino
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        la pastro klini&gcirc;is &gcirc;is la tero, prenis, sen vorto, la <tld/>ulan pladeton kaj
        revenis vila&gcirc;on
        <fnt>
          <bib>IKr</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x53EB;&#x82B1;&#x5B50; <pr>ji&#xE0;ohu&amacron;ze</pr></trd>,
    <trd>&#x4E5E;&#x4E10; <pr>q&#x1D0;g&#xE0;i</pr></trd>,
    <trd>&#x4E5E;&#x98DF; <pr>q&#x1D0;sh&#xED;</pr></trd>,
    <trd>&#x53EB;&#21270;&#x5B50; <pr>ji&#xE0;ohu&amacron;zi</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_zh;&c_a;&c_b;&c_r;&c_a;&c_k;</trd>
  <trd lng="bo">&t_sa;&t_pata;&t_rata;&t_nga;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>
  <trdgrp lng="br">
    <trd>klasker-bara</trd>,
    <trd>klasker-boued</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>almu&zcaron;n&iacute;k</trd>,
    <trd>&zcaron;ebr&aacute;k</trd>
  </trdgrp>
  <trd lng="de">Bettler</trd>
  <trd lng="en">beggar</trd>
  <trdgrp lng="es">
    <trd>limosnero</trd>,
    <trd>pordiosero</trd>,
    <trd>mendigo</trd>
  </trdgrp>
  <trd lng="fr">mendiant</trd>
  <trd lng="he">&qof;&#x5B7;&bet;&#x5BC;&#x5B0;&tsadi;&#x5B8;&fnun;</trd>
  <trdgrp lng="id">
    <trd><baz>kemis</baz> pengemis</trd>,
    <trd>pe<ind>minta</ind>-minta</trd>
  </trdgrp>
  <trd lng="ja">&#x4E5E;&#x98DF;  <pr>&#x307B;&#12356;&#12392;&#12358;</pr></trd>
  <trd lng="nl">bedelaar</trd>
  <trd lng="pl">&zdot;ebrak</trd>
  <trdgrp lng="pt">
    <trd>mendigo</trd>,
    <trd>pedinte</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_i;&c_shch;&c_i;&c_j;</trd>
  <trd lng="sk">&zcaron;obr&aacute;k</trd>
  <trd lng="sv">tiggare</trd>
  <trdgrp lng="uk">
    <trd>&c_zh;&c_je;&c_b;&c_r;&c_a;&#x43A;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_r;&c_je;&c_c;&#x44C;</trd>
  </trdgrp>
</drv>

