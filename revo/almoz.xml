<drv mrk="almoz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="almoz.0o.KOMUNE">
    <dif>
      Mon<ref tip="super" cel="donac.0o">donaco</ref>,
      <ref tip="super" cel="help.mon0o">monhelpo</ref> al malri&ccirc;ulo:
      <ekz>
        blindulo sidis apud la vojo, petante <tld/>ojn
        <fnt>
          <bib>NT</bib>
          <lok>Luko 18:35</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam vi donos <tld/>on, ne sonigu trumpeton anta&ubreve; vi, kiel faras la hipokrituloj
        en la sinagogoj
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 6:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i la unuan fojon etendus la manon por <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        for! al sanaj kaj fortaj mi <tld/>on ne donas
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[dum]</klr> la tuta tago neniu ion a&ccirc;etis de &scirc;i, neniu donis al &scirc;i <tld/>on
        <fnt>
          <bib>Fab2</bib>
          <lok>Knabineto kun alumetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        elpremi bagatelan <tld/>on per strata kantado
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        li trapasis la urban pordegon <klr>[&#8230;]</klr> en la mano brunan <tld/>an telereton
        <fnt>
          <bib>IKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj ne malri&ccirc;igas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x65BD;&#x820D; <pr>sh&#x12B;sh&#x11B;</pr></trd>,
    <trd>&#21608;&#x6D4E; <pr>zh&#x14D;uj&#xEC;</pr></trd>,
    <trd>&#x8CD9;&#x6FDF; <pr>zh&#x14D;uj&#xEC;</pr></trd>,
    <trd>&#x6A80;&#x90A3; <pr>t&#xE1;nn&#xE0;</pr></trd>,
    <trd>&#x4F48;&#x65BD; <pr>b&#xF9;sh&#x12B;</pr></trd>,
    <trd>&#x5E03;&#x65BD; <pr>b&#xF9;sh&#x12B;</pr></trd>,
    <trd>&#x6B8B;&#x7FB9;&#21097;&#x996D; <pr>c&#xE1;ng&emacron;ngsh&#xE8;ngf&#xE0;n</pr></trd>,
    <trd>&#x6B98;&#x7FB9;&#21097;&#x98EF; <pr>c&#xE1;ng&emacron;ngsh&#xE8;ngf&#xE0;n</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ib;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_m;&c_ib;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">aluzen</trd>
  <trdgrp lng="cs">
    <trd>almu&zcaron;na</trd>,
    <trd>milodar</trd>
  </trdgrp>
  <trd lng="de">Almosen</trd>
  <trdgrp lng="en">
    <trd>alms</trd>,
    <trd>charity</trd>,
    <trd>charitable relief</trd>
  </trdgrp>
  <trd lng="es">limosna</trd>
  <trd lng="fr">aum&ocirc;ne</trd>
  <trd lng="he">&nun;&#x5B0;&dalet;&#x5B8;&bet;&#x5B8;&he;</trd>
  <trdgrp lng="id">
    <trd>amal</trd>,
    <trd>derma</trd>,
    <trd>sedekah</trd>
  </trdgrp>
  <trd lng="ja">&#x65BD;&#12375;&#29289;  <pr>&#x307B;&#12393;&#12371;&#12375;&#12418;&#x306E;</pr></trd>
  <trd lng="nl">aalmoes</trd>
  <trd lng="pl">ja&lstroke;mu&zdot;na</trd>
  <trd lng="pt">esmola</trd>
  <trd lng="ro">poman&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_i;&c_l;&c_o;&c_s;&c_t;&c_y;&c_n;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_a;&c_ja;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">almu&zcaron;na</trd>
  <trd lng="sv">allmosa</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_i;&#x43B;&#x43E;&c_s;&c_t;&c_i;&#x43D;&#x44F;</trd>,
    <trd>&#x43F;&#x43E;&c_d;&c_a;&c_ch;&#x43A;&c_a;</trd>
  </trdgrp>
