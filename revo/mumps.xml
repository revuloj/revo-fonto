  </trdgrp>
  <trdgrp lng="ca">
    <trd>galteres</trd>,
    <trd>parotiditis</trd>
  </trdgrp>
  <trd lng="cs">p&rcaron;&iacute;u&scaron;nice</trd>
  <trdgrp lng="de">
