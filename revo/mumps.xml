    <trd>Ziegenpeter</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>oreillons</trd>,
    <trd>parotidite virale</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>beguk</trd>,
    <trd>gondong</trd>,
    <trd>parotitis</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6D41;&#x884C;&#x6027;&#x8033;&#x4E0B;&#x817A;&#x708E;  <pr>&#x308A;&#x3085;&#x3046;&#x3053;&#x3046;&#x305B;&#x3044;&#x3058;&#x304B;&#x305B;&#x3093;&#x3048;&#x3093;</pr></trd>,
