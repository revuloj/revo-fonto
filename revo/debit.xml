<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>debit</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="debit.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Facila podetala a&ubreve; pogranda vendado de varo:
      <ekz>
        tiu komerca&jcirc;o havas grandan <tld/>on.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_b;&c_y;&c_t; <klr tip="amb">(&c_t;&c_a;&c_v;&c_a;&c_r;&c_a;)</klr></trd>
  <trdgrp lng="de">
    <trd>Absatz</trd>,
    <trd>Umsatz</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;bit<klr tip="ind"> (vente de marchandises)</klr></trd>
  <trdgrp lng="hu">
    <trd>kereslet</trd>,
    <trd>kelet<klr tip="ind"> (kereslet)</klr></trd>,
    <trd>kelend&odblac;s&eacute;g</trd>,
  </trdgrp>
  <trd lng="ru">&c_s;&c_b;&c_y;&c_t;</trd>
</drv>

<drv mrk="debit.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Detale disvendadi.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_b;&c_y;&c_v;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_t;&c_a;&c_v;&c_a;&c_r;)</klr></trd>
  <trd lng="fr">d&eacute;biter<klr tip="ind"> (de la marchandise)</klr></trd>
  <trd lng="hu">kicsiben <ind>&aacute;rus&iacute;t</ind></trd>
  <trd lng="ru">&c_s;&c_b;&c_y;&c_v;&c_a;&c_t;&c_mol;<klr tip="amb"> (&c_t;&c_o;&c_v;&c_a;&c_r;)</klr></trd>
</drv>

<drv mrk="debit.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Butiko de podetala vendado.
    </dif>
  </snc>
  <trd lng="fr">d&eacute;bit<klr tip="ind"> (boutique)</klr></trd>
  <trd lng="hu">kiskeresked&eacute;s<klr tip="ind"> (bolt)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_v;&c_o;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_l;&c_a;&c_r;&c_jo;&c_k;</trd>,
    <trd>&c_k;&c_i;&c_o;&c_s;&c_k;</trd>
  </trdgrp>
</drv>
</art>
