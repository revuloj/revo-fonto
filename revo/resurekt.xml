<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: resurekt.xml,v 1.1 2023/05/27 15:22:38 afido Exp $">
    <kap>
      <rad>resurekt</rad>/i
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="resurekt.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">REL</uzo>
  <snc>
    <gra><vspec>ntr</vspec></gra>
    <dif>
      <ref tip="sin" cel="lev.re0igxi.REL">Relevi&gcirc;i</ref> post morto, 
      <ref tip="sin" cel="viv.re0igxi">revivi&gcirc;i</ref>:
      <ekz>
        la <tld/>o de Jesuo Kristo
        <fnt>
          <bib>Monato</bib>
          <aut>Rafaela Urue&ntilde;a</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006809.html"
              >Hispanio: Vojoj al la sankta urbo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <nom>Sherlock Holmes</nom> &ccirc;iam denove <tld/>is
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 3 (53)</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <nom>Halina Gorecka</nom> reliefigas la historiajn traviva&jcirc;ojn, morton kaj <tld/>on de la ondoza
        eldona&jcirc;o el Rusio
        <fnt>
          <bib>Monato</bib>
          <aut>Orlando Raola</aut>
          <vrk><url ref="https://www.monato.be/2013/010150.php"
              >Du gvidiloj al plua legado</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">auferstehen</trd>
    <trd lng="en">resurrect</trd>
    <trd lng="fr">ressusciter</trd>
  </snc>
</drv>

  </art>

