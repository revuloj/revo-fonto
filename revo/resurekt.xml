<vortaro>
  <art mrk="$Id: resurekt.xml,v 1.7 2025/09/21 18:00:21 afido Exp $">
    <kap>
      <rad>resurekt</rad>/i
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="resurekt.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">REL</uzo>
  <snc>
    <gra><vspec>ntr</vspec></gra>
    <dif>
      <ref tip="sin" cel="lev.re0igxi.REL">Relevi&gcirc;i</ref> post morto,
      <ref tip="sin" cel="viv.re0igxi">revivi&gcirc;i</ref>:
      <ekz>
        la <tld/>o de Jesuo Kristo
        <fnt>
          <aut>Rafaela Urue&ntilde;a</aut>
          <vrk><url ref="&Monato;/2000/006809.php">Hispanio: Vojoj al la sankta urbo</url></vrk>
              <bib>Monato</bib>
              <lok>2000/02, p. 9</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <nom>Sherlock Holmes</nom> &ccirc;iam denove <tld/>is
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 3 (53)</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <nom>Halina Gorecka</nom> reliefigas la historiajn traviva&jcirc;ojn, morton kaj <tld/>on de la
        ondoza
        eldona&jcirc;o el Rusio
        <fnt>
          <aut>Orlando Raola</aut>
          <vrk><url ref="&Monato;/2013/010150.php">Du gvidiloj al plua legado</url></vrk>
              <bib>Monato</bib>
          <lok>2013/06, p. 24</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">auferstehen</trd>
    <trd lng="en">resurrect</trd>
    <trd lng="fr">ressusciter</trd>
    <trdgrp lng="nl">
      <trd>verrijzen</trd>,
      <trd>uit de <ind>dood</ind> opstaan</trd>
    </trdgrp>
    <trd lng="pl">wskrzesi&cacute;</trd>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>revivi&gcirc;i</ke>, <ke>relevi&gcirc;i</ke>.
  </rim>

<trd lng="be">&c_u;&c_v;&c_a;&c_s;&c_k;&c_r;&c_a;&c_sh;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">ressuscitar</trd>
  <trd lng="es">ressucitar</trd>
  <trdgrp lng="zh">
    <trd>&#36215;&#27515;&#22238;&#29983; <pr>q&#464;s&#464;hu&iacute;sh&#275;ng</pr></trd>,
    <trd>&#33487;&#29983; <pr>s&#363;sh&#275;ng</pr></trd>,
    <trd>&#34311;&#29983; <pr>s&#363;sh&#275;ng</pr></trd>
  </trdgrp>
</drv>

