<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: aragon.xml,v 1.2 2023/08/30 18:23:36 afido Exp $">
    <kap>
      <rad>Aragon</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="aragon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:regionoj" cel="region.0o.land">Regiono</ref> en
      la nordoriento de <ref tip="malprt" cel="hispan.H0ujo">Hispanujo</ref>:
      <ekz>
        li veturis tra tuta Galegio kaj atingis la hispanan regionon <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Rafaela Urue&ntilde;a</aut>
          <vrk>Hispanio: Vojoj al la sankta urbo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        sur la bazo de la Federacioj kataluna, <tld lit="a"/>a kaj vaska stari&gcirc;is
        Hispana Esperanto-Konfederacio
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili legis nian historion kaj e&ccirc; tiun de la <tld/>ano nove presitan
        <fnt>
          <bib>DKM</bib>
          <lok>&Ccirc;apitro LXI</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Aragon <klr>(Spanien)</klr></trd>
    <trd lng="en">Aragon</trd>
    <trd lng="fr">Aragon</trd>
  </snc>
  <trd lng="ja">&#x30A2;&#x30E9;&#x30B4;&#x30F3;</trd>
</drv>

  </art>

