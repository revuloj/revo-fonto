<vortaro>
<art mrk="$Id$">
  <kap>
  <rad>diskurs</rad>/o
  </kap>
  <drv mrk="diskur.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">BELE</uzo>
    <snc>
      <dif>
      Ordigita, iom longa parolado celanta konvinki. 
      </dif>
      <refgrp tip="vid">
      <ref cel="oraci.0o">oracio</ref>.
      </refgrp>
    </snc>
  <trd lng="kek">aatin</trd>
  <trd lng="be">&c_d;&c_y;&c_s;&c_k;&c_u;&c_r;&c_s;</trd>
  <trd lng="bg">&c_b;&c_je;&c_s;&c_je;&c_d;&c_a;</trd>
  <trd lng="ca">discurs</trd>
  <trdgrp lng="cs">
    <trd>diskurs</trd>,
    <trd>diškurz</trd>,
    <trd>přednáška</trd>,
    <trd>rozprava</trd>,
    <trd>výklad</trd>,
    <trd>řeč</trd>
  </trdgrp>
  <trd lng="de">Diskurs</trd>
  <trd lng="en">discourse</trd>
  <trd lng="es">discurso</trd>
  <trdgrp lng="fr">
      <trd>allocution</trd>,
      <trd>discours<klr tip="ind"> (allocution)</klr></trd></trdgrp>
  <trd lng="hu">sz&oacute;noklat</trd>
  <trd lng="nl">redevoering</trd>
  <trd lng="pl">dyskurs</trd>
  <trd lng="pt">discurso</trd>
  <trdgrp lng="ru">
      <trd>&c_r;&c_je;&c_ch;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_s;&c_t;&c_u;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_d;&c_i;&c_s;&c_k;&c_u;&c_r;&c_s;</trd>
    </trdgrp>
  <trdgrp lng="sk">
    <trd>prednáška</trd>,
    <trd>rozprava</trd>,
    <trd>výklad</trd>
  </trdgrp>
</drv>
</art>
