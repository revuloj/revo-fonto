<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sindrom.xml,v 1.9 2020/05/13 15:48:04 revo Exp $">
<kap>
  <rad>sindrom</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="sindrom.0o">
  <kap><tld/>o</kap>
  <snc mrk="sindrom.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="dif" cel="simpto.0aro.MED">Simptomaro</ref>:
      <ekz>
        anta&ubreve;menstrua <tld/>o
        <fnt>
          <aut>trad. Frank kaj Pa&ubreve;lin'</aut>,
          <vrk><url        
          ref="http://www.geocities.com/Paris/Maison/8040/adin005.html">adoleskantinaj
          demandoj, 5</url></vrk>,  
          <lok>1999</lok>
        </fnt>;
      </ekz>
      <ekz>
        esplori la <tld/>on por trovi la ka&ubreve;zon de malsano
        <fnt>
          <aut>-</aut>,
          <vrk><url 
          ref="http://www.chinareport.com.cn/uk2004/e10-41.htm">&Ccirc;ina 
          Medicino</url></vrk>,
          <lok>[vidita je 2006-10-02]</lok>
        </fnt>;
      </ekz>
      <ekz>
        viruso, provokanta &ccirc;e homo la tiel nomatan <tld/>on de
        imunoperdo, tio estas
        aidoso
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="https://www.monato.be/2004/008160.html"
              >Afriko kaj aidoso</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;sarso&#8221; (Sever-Akuta Respira <tld lit="S"/>o)
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2003/008129.html"
              >Propagando dan&gcirc;era</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        plejparto pereis precipe pro hemoragia <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="https://www.monato.be/2004/008278.html"
              >Damokla glavo super la homaro</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni interpretas la vortojn &leftquot;metabola <tld/>o&#8221; kiel tro da
        interna&jcirc;a graso
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Yasuo</aut>
          <vrk><url ref="https://www.monato.be/2009/009700.php"
              >Dikuloj maldiki&gcirc;u!</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu religia diskriminacio ne limi&gcirc;as al ia &leftquot;<tld/>o post la 11a
        de septembro&#8221;
        <fnt>
          <bib>Monato</bib>
          <aut>Istvan Ertl</aut>
          <vrk><url ref="https://www.monato.be/2003/008103.html"
              >Dio en la konstitucio, diablo en la detaloj</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="morb.0o">morbo</ref>
  </snc>
  <trdgrp lng="cs">
    <trd>soubor p&rcaron;&iacute;znak&uring;</trd>,
    <trd>syndrom</trd>
  </trdgrp>
  <trd lng="de">Syndrom</trd>
  <trd lng="fr">syndrome</trd>
  <trd lng="it">sindrome</trd>
  <trd lng="nl">syndroom</trd>
  <trdgrp lng="sk">
    <trd>syndr&oacute;m</trd>,
    <trd>s&uacute;bor pr&iacute;znakov</trd>
  </trdgrp>
</drv>
</art>

