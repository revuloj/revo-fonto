<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sindrom.xml,v 1.17 2025/04/05 16:29:36 revo Exp $">
<kap>
  <rad>sindrom</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="sindrom.0o">
  <kap><tld/>o</kap>
  <snc mrk="sindrom.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="dif" cel="simpto.0aro.MED">Simptomaro</ref>:
      <ekz>
        anta&ubreve;menstrua <tld/>o
        <fnt>
          <aut>trad. Frank kaj Pa&ubreve;lin'</aut>,
          <vrk><url ref="http://www.geocities.com/Paris/Maison/8040/adin005.html">adoleskantinaj
          demandoj, 5</url></vrk>,
          <lok>1999</lok>
        </fnt>;
      </ekz>
      <ekz>
        esplori la <tld/>on por trovi la ka&ubreve;zon de malsano
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.chinareport.com.cn/uk2004/e10-41.htm">&Ccirc;ina
          Medicino</url></vrk>,
          <lok>[vidita je 2006-10-02]</lok>
        </fnt>;
      </ekz>
      <ekz>
        viruso, provokanta &ccirc;e homo la tiel nomatan <tld/>on de
        imunoperdo, tio estas
        aidoso
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2004/008160.html">Afriko kaj aidoso</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;sarso&#8221; (Sever-Akuta Respira <tld lit="S"/>o)
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2003/008129.html">Propagando dan&gcirc;era</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        plejparto pereis precipe pro hemoragia <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2004/008278.html">Damokla glavo super la homaro</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni interpretas la vortojn &leftquot;metabola <tld/>o&#8221; kiel tro da
        interna&jcirc;a graso
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Yasuo</aut>
          <vrk><url ref="&Monato;/2009/009700.php">Dikuloj maldiki&gcirc;u!</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu religia diskriminacio ne limi&gcirc;as al ia &leftquot;<tld/>o post la 11a
        de septembro&#8221;
        <fnt>
          <bib>Monato</bib>
          <aut>Istvan Ertl</aut>
          <vrk><url ref="&Monato;/2003/008103.html">Dio en la konstitucio, diablo en la
          detaloj</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="morb.0o">morbo</ref>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>simptomaro</ke>.
  </rim>

  <trd lng="be">&c_s;&c_y;&c_n;&c_d;&c_r;&c_o;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>soubor p&rcaron;&iacute;znak&uring;</trd>,
    <trd>syndrom</trd>
  </trdgrp>
  <trd lng="de">Syndrom</trd>
  <trd lng="fr">syndrome</trd>
  <trd lng="it">sindrome</trd>
  <trd lng="ja">&#30151;&#20505;&#32676;  <pr>&#12375;&#12423;&#12358;&#12371;&#12358;&#12368;&#12435;</pr></trd>
  <trd lng="nl">syndroom</trd>
  <trd lng="pl">syndrom</trd>
  <trdgrp lng="sk">
    <trd>syndr&oacute;m</trd>,
    <trd>s&uacute;bor pr&iacute;znakov</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&#1085;&c_d;&c_r;&#1086;&#1084;</trd>,
    <trd>&#1082;&#1086;&#1084;&#1087;&#1083;&c_je;&#1082;&c_s; &c_s;&c_i;&#1084;&#1087;&c_t;&#1086;&#1084;&c_ib;&c_v;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#32156;&#21512;&#30151; <pr>z&#333;ngh&#233;zh&#232;ng</pr></trd>,
    <trd>&#32508;&#21512;&#30151; <pr>z&#333;ngh&#233;zh&#232;ng</pr></trd>,
    <trd>&#32156;&#21512;&#24501; <pr>z&#333;ngh&#233;zh&#275;ng</pr></trd>,
    <trd>&#32508;&#21512;&#24449; <pr>z&#333;ngh&#233;zh&#275;ng</pr></trd>,
    <trd>&#20341;&#30332;&#30151;&#29376; <pr>b&#236;ngf&#257;zh&#232;ngzhu&#224;ng</pr></trd>,
    <trd>&#24182;&#21457;&#30151;&#29366; <pr>b&#236;ngf&#257;zh&#232;ngzhu&#224;ng</pr></trd>,
    <trd>&#20986;&#24046;&#37679; <pr>ch&#363;ch&#257;cu&#242;</pr></trd>,
    <trd>&#20986;&#24046;&#38169; <pr>ch&#363;ch&#257;cu&#242;</pr></trd>,
    <trd>&#21516;&#26102;&#23384;&#22312;&#30340;&#20107;&#29289; <pr>t&#243;ngsh&#237;c&#250;nz&#224;idesh&#236;w&#249;</pr></trd>,
    <trd>&#21516;&#26178;&#23384;&#22312;&#30340;&#20107;&#29289; <pr>t&#243;ngsh&#237;c&#250;nz&#224;idesh&#236;w&#249;</pr></trd>,
    <trd>&#30151;&#20505;&#32676; <pr>zh&#232;ngh&#242;uq&#250;n</pr></trd>,
    <trd>&#32156;&#21512;&#24449; <pr>z&#333;ngh&#233;zh&#232;ng</pr></trd>,
    <trd>&#32508;&#21512;&#24449; <pr>z&#333;ngh&#233;zh&#232;ng</pr></trd>
  </trdgrp>
</drv>
