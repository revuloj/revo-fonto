<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sindrom.xml,v 1.14 2023/09/03 18:39:30 revo Exp $">
<kap>
  <rad>sindrom</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="sindrom.0o">
  <kap><tld/>o</kap>
  <snc mrk="sindrom.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="dif" cel="simpto.0aro.MED">Simptomaro</ref>:
      <ekz>
        anta&ubreve;menstrua <tld/>o
        <fnt>
          <aut>trad. Frank kaj Pa&ubreve;lin'</aut>,
          <vrk><url ref="http://www.geocities.com/Paris/Maison/8040/adin005.html">adoleskantinaj
          demandoj, 5</url></vrk>,
          <lok>1999</lok>
        </fnt>;
      </ekz>
      <ekz>
        esplori la <tld/>on por trovi la ka&ubreve;zon de malsano
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.chinareport.com.cn/uk2004/e10-41.htm">&Ccirc;ina
          Medicino</url></vrk>,
          <lok>[vidita je 2006-10-02]</lok>
        </fnt>;
      </ekz>
      <ekz>
        viruso, provokanta &ccirc;e homo la tiel nomatan <tld/>on de
        imunoperdo, tio estas
        aidoso
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2004/008160.html">Afriko kaj aidoso</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;sarso&#x201D; (Sever-Akuta Respira <tld lit="S"/>o)
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2003/008129.html">Propagando dan&gcirc;era</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        plejparto pereis precipe pro hemoragia <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2004/008278.html">Damokla glavo super la homaro</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni interpretas la vortojn &leftquot;metabola <tld/>o&#x201D; kiel tro da
        interna&jcirc;a graso
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Yasuo</aut>
          <vrk><url ref="&Monato;/2009/009700.php">Dikuloj maldiki&gcirc;u!</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu religia diskriminacio ne limi&gcirc;as al ia &leftquot;<tld/>o post la 11a
        de septembro&#x201D;
        <fnt>
          <bib>Monato</bib>
          <aut>Istvan Ertl</aut>
          <vrk><url ref="&Monato;/2003/008103.html">Dio en la konstitucio, diablo en la
          detaloj</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="morb.0o">morbo</ref>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_n;&c_d;&c_r;&c_o;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>soubor p&rcaron;&iacute;znak&uring;</trd>,
    <trd>syndrom</trd>
  </trdgrp>
  <trd lng="de">Syndrom</trd>
  <trd lng="fr">syndrome</trd>
  <trd lng="it">sindrome</trd>
  <trd lng="ja">&#x75C7;&#x5019;&#x7FA4;  [<ind>&#x3057;&#x3087;&#x3046;&#x3053;&#x3046;&#x3050;&#x3093;</ind>]</trd>
  <trd lng="nl">syndroom</trd>
  <trd lng="pl">syndrom</trd>
  <trdgrp lng="sk">
    <trd>syndr&oacute;m</trd>,
    <trd>s&uacute;bor pr&iacute;znakov</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&#x43D;&c_d;&c_r;&#x43E;&#x43C;</trd>,
    <trd>&#x43A;&#x43E;&#x43C;&#x43F;&#x43B;&c_je;&#x43A;&c_s; &c_s;&c_i;&#x43C;&#x43F;&c_t;&#x43E;&#x43C;&c_ib;&c_v;</trd>
  </trdgrp>
</drv>
</art>

