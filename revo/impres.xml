<drv mrk="impres.0ismo">
  <kap>
    <tld/>ismo
  </kap>
  <snc mrk="impres.0ismo.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Arta, precipe pentrarta <ref tip="super" cel="skol.0o">skolo</ref>, aperinta fine de la
      19a jarcento, en kiu la artisto kaptas kaj redonas &gcirc;eneralan <tld/>on de objektoj
      en ia difinita momento, vidataj sub ia difinita lumo;
      la nomon al la skolo donis la pentra&jcirc;o de
      franca pentristo <nom>C. Monet</nom> <ctl><tld lit="I"/>o, levi&gcirc;anta suno</ctl>, 1872;
      <ref tip="sin" cel="impres2.0o.BELA">impresionismo</ref>:
      <ekz>
        preska&ubreve; <tld/>isma efiko
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ismaj pentristoj volas redoni naturajn, lokatmosferajn, socirilatajn kaj
        similajn impresojn kvaza&ubreve; nur per lumo kaj koloro
        <fnt>
          <aut>L. C. Deij</aut>
          <vrk><url ref="http://www.tekstoj.nl/mr/MR473.pdf">Enkonduko al Beletro</url></vrk>
          <lok>Malgranda Revuo, 1947:3</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_m;&c_p;&c_r;&c_e;&c_s;&c_ib;&c_ja;&c_n;&c_ib;&c_z;&c_m;</trd>
  <trd lng="bg">&c_i;&c_m;&c_p;&c_r;&c_je;&c_s;&c_i;&c_o;&c_n;&c_i;&c_z;&c_malmol;&c_m;</trd>
  <trd lng="cs">impresionizmus</trd>
  <trd lng="de">Impressionismus</trd>
  <trd lng="en">impressionism</trd>
  <trd lng="es">impresionismo</trd>
  <trd lng="fr">impressionnisme</trd>
  <trd lng="hu">impresszionizmus</trd>
  <trd lng="nl">impressionisme</trd>
  <trd lng="pt">impressionismo</trd>
  <trd lng="ru">&c_i;&c_m;&c_p;&c_r;&c_je;&c_s;&c_s;&c_i;&c_o;&c_n;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">impresionizmus</trd>
  <trd lng="zh">&#x5370;&#x8C61;&#x4E3B;&#x4E49; <pr>y&#xEC;nxi&#xE0;ngzh&#x1D4;y&#xEC;</pr></trd>
