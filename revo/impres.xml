<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>impres</rad>/o
</kap>
<drv mrk="impres.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="impres.0o.gxenerale">
    <dif>
      Influo, efiko, farita sur la sentumoj, la koro a&ubreve; la spirito:
      <ekz>
        senti <tld/>on de malvarmo;
      </ekz>
      <ekz>
        oni havis la <tld/>on kvaza&ubreve; &ccirc;io dormas<fnt>Z</fnt>;
      </ekz>
      <ekz>
        sub la <tld/>o de teruro, li forlasis la postenon;
      </ekz>
      <ekz>
        tiu parolado, pentra&jcirc;o, virino faris sur min potencan,
        profundan <tld/>on, nur malfortan <tld/>on;
      </ekz>
      <ekz>
        nia nombro faris <tld/>on.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="efekt.0o">efekto</ref>,
      <ref cel="impon">impono</ref>.
    </refgrp>
    <trdgrp lng="fr">
      <trd>impression<klr tip="ind"> (sensation)</klr></trd>,
      <trd>sensation<klr tip="ind"> (impression)</klr></trd></trdgrp> 
    <trd lng="nl">indruk</trd>
    <trd lng="ru">&c_v;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="impres.0o.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      Fizika efiko de lumradioj sur fotografa plato:
      <ekz>
        tempo de <tld/>o.
      </ekz>
    </dif>
    <trd lng="fr">impression<klr tip="ind"> (phot.)</klr></trd>
    <trd lng="nl">belichting</trd>
  </snc>
</drv>

<drv mrk="impres.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Estigi, fari <tld/>on sur:
      <ekz>
        la akcidento, kongreso profunde <tld/>is lin;
      </ekz>
      <ekz>
        tiu ceremonio <tld/>is min neforgeseble;
      </ekz>
      <ekz>
        favore, melankolie <tld/>i iun;
      </ekz>
      <ekz>
        lia aserto <tld/>as kiel paradokso;
      </ekz>
      <ekz>
        <tld/>ita de la admono.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gravur.0i">gravuri</ref>.
    </refgrp>
  </snc>
  <adm>
  Aldoni ekz-on FOT. <aut>MB</aut>
  </adm>
  <trdgrp lng="fr">
    <trd>impressionner<klr tip="amb"> (produire une impression)</klr></trd>,
    <trd>impressionner<klr tip="amb"> (photographiquement)</klr></trd>,
    <trd>donner l'<ind>impression</ind> (de)</trd></trdgrp> 
  <trd lng="nl">indruk maken</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_i; &c_v;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="impres.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Faranta fortan <tld/>on:
      <ekz>
        <tld/>a spektaklo, rakonto, propagandilo.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">impresionnant</trd>
  <trd lng="nl">indruk makend</trd>
  <trd lng="ru">&c_v;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_l;&c_ja;&c_ju;&c_shch;&c_i;&c_j;</trd>
</drv>

<drv mrk="impres.0ebla0igxema">
  <kap><tld/>ebla, <tld/>i&gcirc;ema</kap>
  <snc>
    <dif>
      Povanta esti facile <tld/>ata:
      <ekz>
        <tld/>i&gcirc;ema karaktero, knabo.
      </ekz>
    </dif>
    <trd lng="ru">&c_v;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_l;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc>
    <uzo tip="fak">FOT</uzo>
    <dif>
      Preparita por ricevi la efikon de la lumradioj:
      <ekz>
        <tld/>i&gcirc;ema fotografa plato.
      </ekz>
    </dif>
    <trd lng="nl">gevoelig<klr tip="ind"> (fot.)</klr></trd>
    <trd lng="ru">&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trd lng="fr">impressionnable</trd>
</drv>
</art>
