<drv mrk="impres.malbon0a">
  <kap>
    malbon<tld/>a,
    <var><kap>mis<tld/>a</kap></var>
  </kap>
  <snc mrk="impres.malbon0a.KOMUNE">
    <dif>
      Kiu ka&ubreve;zas malbonan <tld/>on, &scirc;ajnas <ref cel="bon.mal0a">malbona</ref>,
      <ref cel="sxok.0a.KOMUNE">&scirc;oka</ref>, neakceptebla:
      <ekz>
        en la profundo staras malserenaj, malbon<tld/>aj domoj kun lignaj palisaroj
        <fnt>
          <bib>Fab2</bib>
          <lok>Bildo el kastelremparo</lok>
        </fnt>;
      </ekz>
      <ekz>
        pasintece mis<tld/>aj ilustra&jcirc;oj
        <fnt>
          <aut>M. Nervi</aut>
          <vrk><url ref="http://retbutiko.be/de/ero/purg">
          La plej granda verko iam ajn verkita</url></vrk>
          <lok>Esperanto, 2003-12</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="abomen.0a">abomena</ref>,
      <ref cel="bel.mal0a">malbela</ref>,
      <ref cel="kaduk.0a.ruinigxa">kaduka</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ja;&c_k;&c_ib; <ind>&c_n;&c_je;&c_p;&c_r;&c_y;&c_je;&c_m;&c_n;&c_a;</ind> &c_w;&c_r;&c_a;&c_zh;&c_v;&c_a;&c_je;</trd>,
    <trd>&c_a;&c_g;&c_ib;&c_d;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">einen schlechten <ind>Eindruck</ind> hinterlassend</trd>
  <trdgrp lng="en">
    <trd>abhorrent</trd>,
    <trd>loathsome</trd>,
    <trd>offensive</trd>,
    <trd>yucky</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>de mauvais aloi</trd>,
    <trd>de mauvais effet</trd>
  </trdgrp>
</drv>
