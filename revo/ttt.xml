<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>TTT</rad>
    </kap>
    <drv mrk="ttt.0">
      <kap><tld/></kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Mallongigo de <ctl>Tut-Tera Teksa&jcirc;o</ctl>,
          <ref cel="tekst.hiper0o">hiperteksta</ref> sistemo en la
          <ref tip="malprt" cel="ret.inter0o.la">Interreto</ref>.
        </dif>
      </snc>
      <rim>
        Legu <ctl>to-to-to</ctl>.
      </rim>
      <trdgrp lng="en"><trd>WWW</trd>, <trd>Web</trd></trdgrp>
      <trdgrp lng="ru"><trd>&#x412;&#x441;&#x435;&#x43c;&#x438;&#x440;&#x43d;&#x430;&#x44f; <ind>&#x41f;&#x430;&#x443;&#x442;&#x438;&#x43d;&#x430;</ind></trd>,
        <trd>&#x41f;&#x41f;&#x41f; <klr>(&#x41f;&#x43e;&#x432;&#x441;&#x435;&#x43c;&#x435;&#x441;&#x442;&#x43d;&#x43e; &#x41f;&#x440;&#x43e;&#x442;&#x44f;&#x43d;&#x443;&#x442;&#x430;&#x44f; &#x41f;&#x430;&#x443;&#x442;&#x438;&#x43d;&#x430;)</klr></trd>,
        <trd>&#x412;&#x435;&#x431;</trd></trdgrp>
    </drv>
    <drv mrk="ttt.0ejo">
      <kap><tld/>-ejo</kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Parto de <ref tip="malprt" cel="ttt.0">TTT</ref>, retnodo 
          disponiganta rete atingeblajn hipertekstojn la&ubreve; la normoj de
          TTT.
        </dif>
      </snc>
      <trd lng="en">Web site</trd>
      <trd lng="ru">&#x441;&#x430;&#x439;&#x442;</trd>
    </drv>
    <drv mrk="ttt.0pagxo">
      <kap><tld/>-pa&gcirc;o</kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Peco de hiperteksto trovebla en
          <ref tip="malprt" cel="ttt.0">TTT</ref> per retadreso
          (URL).
        </dif>
      </snc>
      <trd lng="en">Web page</trd>
      <trd lng="ru">&#x441;&#x442;&#x440;&#x430;&#x43d;&#x438;&#x447;&#x43a;&#x430; &#x432; &#x418;&#x43d;&#x442;&#x435;&#x440;&#x43d;&#x435;&#x442;&#x435;</trd>
    </drv>
    <drv mrk="ttt.0pagxaro">
      <kap><tld/>-pa&gcirc;aro</kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Kolekto da <ref tip="prt" cel="ttt.0pagxo">TTT-pa&gcirc;oj</ref>,
          precipe se ili havas komunan temaron a&ubreve; a&ubreve;toron.
        </dif>
      </snc>
      <trdgrp lng="en"><trd><klr>(someone's)</klr> <ind>Web pages</ind></trd>,
        <trd><klr>(someone's)</klr> <ind>Web site</ind></trd></trdgrp>
    </drv>
  </art>

