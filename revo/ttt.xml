<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>TTT</rad>
    </kap>
    <drv mrk="ttt.0">
      <kap><tld/></kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Mallongigo de <ctl>Tut-Tera Teksa&jcirc;o</ctl>,
          <ref cel="tekst.hiper0o">hiperteksta</ref> sistemo en la
          <ref tip="malprt" cel="ret.inter0o.la">Interreto</ref>.
        </dif>
      </snc>
      <rim>
        Legu <ctl>to to to</ctl>.
      </rim>
      <trdgrp lng="be">
        <trd>www</trd>,
        <trd>&c_s;&c_u;&c_s;&c_mol;&c_v;&c_je;&c_t;&c_n;&c_a;&c_je; <ind>&c_p;&c_a;&c_v;&c_u;&c_ch;&c_y;&c_n;&c_mol;&c_n;&c_je;</ind></trd>,
        <trd>&c_v;&c_e;&c_b;</trd>
      </trdgrp>
      <trd lng="de">WWW</trd>
      <trdgrp lng="en">
       <trd>WWW</trd>, 
       <trd>Web</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>vil&aacute;gh&aacute;l&oacute;</trd>,
        <trd>www</trd>
        <trd>web</trd>
      </trdgrp>
      <trdgrp lng="nl">
       <trd>WWW</trd>, 
       <trd>web</trd>
      </trdgrp>
      <trdgrp lng="ru"><trd>&c_V;&c_s;&c_je;&c_m;&c_i;&c_r;&c_n;&c_a;&c_ja; <ind>&c_P;&c_a;&c_u;&c_t;&c_i;&c_n;&c_a;</ind></trd>,
        <trd>&c_P;&c_P;&c_P; <klr>(&c_P;&c_o;&c_v;&c_s;&c_je;&c_m;&c_je;&c_s;&c_t;&c_n;&c_o; &c_P;&c_r;&c_o;&c_t;&c_ja;&c_n;&c_u;&c_t;&c_a;&c_ja; &c_P;&c_a;&c_u;&c_t;&c_i;&c_n;&c_a;)</klr></trd>,
        <trd>&c_V;&c_je;&c_b;</trd></trdgrp>
    </drv>

    <drv mrk="ttt.0ejo">
      <kap><tld/>-ejo</kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Parto de <ref tip="malprt" cel="ttt.0">TTT</ref>, retnodo 
          disponiganta rete atingeblajn hipertekstojn la&ubreve; la normoj de
          TTT.
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_s;&c_a;&c_j;&c_t;</trd>,
        <trd>&c_v;&c_e;&c_b;-&c_s;&c_a;&c_j;&c_t;</trd>
      </trdgrp>
      <trd lng="en">web site</trd>
      <trdgrp lng="hu">
       <trd>h&aacute;l&oacute;hely</trd>, 
       <trd>web-hely</trd>,
       <trd>web-site</trd>
      </trdgrp>      
      <trdgrp lng="nl">
       <trd>webstek</trd>, 
       <trd>web site</trd>
      </trdgrp>
      <trd lng="pl">strona <ind>internetowa</ind></trd>
      <trd lng="ru">&c_s;&c_a;&c_j;&c_t;</trd>
    </drv>

    <drv mrk="ttt.0pagxo">
      <kap><tld/>-pa&gcirc;o</kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Peco de hiperteksto trovebla en
          <ref tip="malprt" cel="ttt.0">TTT</ref> per retadreso
          (URL).
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_s;&c_t;&c_a;&c_r;&c_o;&c_n;&c_k;&c_a; &c_w; &c_Ib;&c_n;&c_t;&c_e;&c_r;&c_n;&c_e;&c_c;&c_je;</trd>,
        <trd>&c_v;&c_e;&c_b;-&c_s;&c_t;&c_a;&c_r;&c_o;&c_n;&c_k;&c_a;</trd>
      </trdgrp>
      <trd lng="en">web page</trd>
      <trdgrp lng="hu">
       <trd>h&aacute;l&oacute;lap</trd>, 
       <trd>weblap</trd>,
       <trd>web-page</trd>
      </trdgrp>      
      <trd lng="nl">webbladzijde</trd>
      <trd lng="ru">&c_s;&c_t;&c_r;&c_a;&c_n;&c_i;&c_ch;&c_k;&c_a; &c_v; &c_I;&c_n;&c_t;&c_je;&c_r;&c_n;&c_je;&c_t;&c_je;</trd>
    </drv>

    <drv mrk="ttt.0pagxaro">
      <kap><tld/>-pa&gcirc;aro</kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Kolekto da <ref tip="prt" cel="ttt.0pagxo">TTT-pa&gcirc;oj</ref>,
          precipe se ili havas komunan temaron a&ubreve; a&ubreve;toron.
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_s;&c_a;&c_j;&c_t;</trd>,
        <trd>&c_v;&c_e;&c_b;-&c_s;&c_a;&c_j;&c_t;</trd>
      </trdgrp>
      <trdgrp lng="en"><trd>web site</trd></trdgrp>
      <trdgrp lng="hu">
       <trd>h&aacute;l&oacute;hely</trd>, 
       <trd>h&aacute;l&oacute;lapok</trd>,
       <trd>web-lapok</trd>
      </trdgrp>      
      <trdgrp lng="nl">
       <trd>webstek</trd>, 
       <trd>web site</trd>
      </trdgrp>
     <trd lng="pl">strona <ind>internetowa</ind></trd>
</drv>
  </art>

