<vortaro>
<art mrk="$Id: diater.xml,v 1.13 2020/05/21 06:35:25 revo Exp $">
  <kap>
    <rad>diaterm</rad>/a
  </kap>
<drv mrk="diater.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc mrk="diater.0a.FIZ">
    <dif>
      Translasanta la varmon:
      <ekz>
        la solidaj kaj likvaj materioj estas nur malofte <tld/>aj
        <fnt>
          <aut>Miroslav T&uring;ma</aut>
          <vrk><url ref="https://eo.wikibooks.org/wiki/Termodinamiko/Leciono_9"
              >Termodinamiko</url></vrk>
          <lok>Leciono 9</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd
    lng="be">&c_d;&c_y;&c_ja;&c_t;&c_e;&c_r;&c_m;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>diaterm&aacute;ln&iacute;</trd>,
    <trd>pr&uring;tepliv&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>diatherm</trd>,
      <trd>w&auml;rmedurchl&auml;ssig</trd>
    </trdgrp>
  <trdgrp lng="en">
      <trd>diathermic</trd>,
      <trd>diathermal</trd>,
      <trd>diathermanous</trd>
    </trdgrp>
  <trdgrp lng="hu">
      <trd>diaterm&aacute;n</trd>,
      <trd>diatermikus</trd>,
      <trd>h&odblac;&aacute;tereszt&odblac;</trd>
    </trdgrp>
  <trdgrp lng="pl">
      <trd>termoprzewodni</trd>
    </trdgrp>
  <trd lng="pt">diat&eacute;rmico</trd>
  <trd lng="sk">diaterm&aacute;lny</trd>
</drv>
</art>
