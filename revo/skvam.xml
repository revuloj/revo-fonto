<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: skvam.xml,v 1.36 2024/03/25 06:40:51 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>skvam</rad>/o
</kap>
<drv mrk="skvam.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="skvam.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Iu el la kornecaj lamenetoj, kiuj tegas la korpon de plej multaj
      fi&scirc;oj kaj rampuloj:
      <ekz>
        belaj fi&scirc;oj kun ar&gcirc;entaj kaj oraj <tld/>oj na&gcirc;is
        post la boato
        <fnt><bib>Fab2</bib> <lok>Dio de dormo, Mardo.</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="nl">schub</trd>
  </snc>
  <snc mrk="skvam.0o.FIG">
    <dif>
      Surfaca ero simila al <tld/>o:
      <ekz>
        &ccirc;e kelkaj vegeta&jcirc;oj <tld/>oj anstata&ubreve;as foliojn;
      </ekz>
      <ekz>
        skarlatinaj <tld/>oj de la ha&ubreve;to;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ho, se la <tld/>oj defalus de viaj okuloj<fnt>Z</fnt>!
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        per <tld/>ita kiraso li estis vestita
        <fnt><bib>MT</bib> <lok>&Sa1; 17:5</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="ca">esquama <klr>(vegetals)</klr></trd>
    <trd lng="nl">schilfer</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_u;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_l;&c_u;&c_s;&c_k;&c_a;&c_v;&c_ib;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_l;&c_ju;&c_s;&c_p;&c_a;</trd>
  <trd lng="ca">escata</trd>
  <trdgrp lng="cs">
    <trd>z&aacute;voj</trd>,
    <trd>&scaron;upina</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Fischschuppe</trd>
      <trd>Schuppe</trd>
  </trdgrp>
  <trd lng="en">scale<klr> (of fish, etc)</klr></trd>
  <trd lng="es">escama</trd>
  <trd lng="fr">&eacute;caille</trd>
  <trd lng="hu">pikkely</trd>
  <trdgrp lng="ja">
    <trd>&#x3046;&#x308D;&#x3053;</trd>,
    <trd>&#x9C57;&#x7C89;  <pr>&#x308A;&#x3093;&#x3077;&#x3093;</pr></trd>,
    <trd>&#x9C57;&#x7247;  <pr>&#x308A;&#x3093;&#x307A;&#x3093;</pr></trd>,
    <trd>&#x9C57;&#x5C51;  <pr>&#x308A;&#x3093;&#x305B;&#x3064;</pr></trd>
  </trdgrp>
  <trd lng="nl">schub</trd>
  <trd lng="pl">&lstroke;uska</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_sh;&c_u;&c_ja;</trd>,
    <trd>&c_ch;&c_je;&c_sh;&c_u;&c_j;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">&scaron;upina <klr>(na ryb&aacute;ch)</klr></trd>
  <trd lng="sv">fj&auml;ll<klr tip="amb"> (p&aring; fiskar t.ex.)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_l;&c_u;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_l;&c_u;&c_s;&c_o;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_l;&c_u;&c_sh;&c_p;&c_a;&c_j;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_l;&c_ib;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_l;&c_u;&c_z;&c_g;&c_a;</trd>,
    <trd>&c_l;&c_u;&c_p;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_l;&c_je;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9C57;&#x7247; <pr>l&#xED;npi&#xE0;n</pr></trd>,
    <trd>&#x9CDE;&#x7247; <pr>l&#xED;npi&#xE0;n</pr></trd>,
    <trd>&#x9C57; <pr>l&#xED;n</pr></trd>,
    <trd>&#x9CDE; <pr>l&#xED;n</pr></trd>,
    <trd>&#x9B5A;&#x9C57; <pr>y&#xFA;l&#xED;n</pr></trd>,
    <trd>&#x9C7C;&#x9CDE; <pr>y&#xFA;l&#xED;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="skvam.de0igi">
  <kap>de<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="skvam.de0igi.KOMUNE">
    <dif>
      Disigi materian tavolon per estigo kaj forigo de <tld/>oj:
      <ekz>
        kugloj de<tld/>igis stuka&jcirc;on
        <fnt>
          <aut>G. G&aacute;rdonyi, trad. J. Horvath</aut>
          <vrk><url ref="http://vortaro.hu/stelspec.pdf">
          Steloj de Eger</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_s;&c_k;&c_r;&c_a;&c_b;&c_a;&c_c;&c_mol; <ind>&c_l;&c_u;&c_s;&c_k;&c_u;</ind></trd>,
    <trd>&c_a;&c_ch;&c_y;&c_sh;&c_ch;&c_a;&c_c;&c_mol; &c_a;&c_d; <ind>&c_l;&c_u;&c_s;&c_k;&c_ib;</ind></trd>
  </trdgrp>
  <trd lng="de">entschuppen</trd>
  <trdgrp lng="en">
    <trd>scale<klr> (remove the scales of)</klr></trd>,
    <trd>descale</trd>
  </trdgrp>
  <trd lng="fr">&eacute;cailler</trd>
  <trd lng="nl">ontschubben</trd>
</drv>

<drv mrk="skvam.0uloj">
  <kap><tld/>uloj</kap>
  <snc mrk="skvam.0uloj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref>
      de <ref tip="malprt" cel="reptil.0oj">reptilioj</ref>
      (<trd lng="la">Squamata</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_u;&c_s;&c_k;&c_a;&c_v;&c_a;&c_t;&c_y;&c_ja; <klr>(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="bg">&c_l;&c_ju;&c_s;&c_p;&c_je;&c_s;&c_t;&c_i;</trd>
  <trd lng="ca">escamosos</trd>
  <trd lng="de">Schuppenkriechtiere</trd>
  <trdgrp lng="en">
    <trd>squamates</trd>,
    <trd>scaled reptiles</trd>
  </trdgrp>
  <trd lng="fr">squamates</trd>
  <trd lng="hu">pikkelyes <ind>h&uuml;ll&odblac;k</ind></trd>
  <trd lng="nl">squamata</trd>
  <trdgrp lng="pl">
    <trd>&lstroke;uskosk&oacute;re</trd>,
    <trd>&lstroke;uskono&sacute;ne</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_je;&c_sh;&c_u;&c_j;&c_ch;&c_a;&c_t;&c_y;&c_je; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trd lng="uk">&c_l;&c_u;&c_s;&c_k;&c_a;&c_t;&c_ib;</trd>
  <trdgrp lng="zh">
    <trd>&#x6709;&#x9C57;&#x76EE; <pr>y&#x1D2;ul&#xED;nm&#xF9;</pr></trd>,
    <trd>&#x6709;&#x9CDE;&#x76EE; <pr>y&#x1D2;ul&#xED;nm&#xF9;</pr></trd>
  </trdgrp>
</drv>
</art>
