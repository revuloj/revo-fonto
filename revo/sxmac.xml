<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxmac.xml,v 1.21 2023/12/09 08:27:11 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;mac</rad>/i
  <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="sxmac.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="sxmac.0.KOMUNE">
    <dif>
      Bruo kiel &ccirc;e subita lipmalfermo (kiso, ma&ccirc;o):
      <ekz>
        <tld lit="&Scirc;"/>', survango a&ubreve;di&gcirc;is en vic-ordo tia, &parsl;
        Kiel vorto post vorto, unu post alia
        <fnt>
          <aut>A. Mickiewicz, trad. A. Grabowski</aut>
          <vrk>Sinjoro Tadeo</vrk>
          <lok>1918</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i kisetas la genuon de Jo&ccirc;jo (<tld/>')
        <fnt>
           <aut>[R. Cash?]</aut>
          <vrk><url ref="https://soc.culture.esperanto.narkive.com/VJlsagGL/rideto-fin-julia">
          Rideto fin-julia </url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_m;&c_ja;&c_k;!</trd>,
    <trd>&c_c;&c_m;&c_o;&c_k;!</trd>,
    <trd>&c_sh;&c_m;&c_ja;&c_k;!</trd>
  </trdgrp>
  <trd lng="de">schmatz!</trd>
  <trd lng="fr">chmou&iuml;c</trd>
  <trd lng="pl">cmok</trd>
</drv>

<drv mrk="sxmac.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc mrk="sxmac.0i.brui">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      <ref tip="super" cel="bru.0i.soni">Brui</ref> kiel &ccirc;e subita malfermo
      de la <ref cel="lip.0o.ANA_busxa">lipo</ref>j,
      ekz-e &ccirc;e forta <ref cel="kis.0i.FIZL">kiso</ref>,
      brua man&gcirc;ado, eltiro de piedo el &scirc;limo k.s.:
      <ekz>
        li time kra&ccirc;otusis kaj <tld/>is per la lipoj
        <fnt>
          <aut>A. &Ccirc;e&hcirc;ov, trad. K. Bein</aut>
          <vrk>Patro</vrk>
          <lok>Internacia krestomatio, 1907</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pezaj pa&scirc;oj nun preteriris la domon,
        nur la malla&ubreve;ti&gcirc;anta <tld/>o de la koto a&ubreve;di&gcirc;is
        <fnt>
          <bib>LGA</bib>
          <lok>V.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> per avida <tld/>o fumadis dum studentaj jaroj
        <fnt>
          <bib>LSB</bib>
          <lok>Anta&ubreve;signoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam Leopoldo vidis, ke sur la frunto de lia edzino pasas unu el tiuj ombroj,
        kies ka&ubreve;zon li tro konis, li havis neniun malfacilecon por &gcirc;in vaporigi per
        blovo de <tld/>ado
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      lango<ref cel="klak.0i.KOMUNE">klaki</ref>,
      <ref cel="macx.0i.ANA">ma&ccirc;i</ref>,
      <ref cel="sucx.0i">su&ccirc;i</ref>
    </refgrp>
  </snc>
  <snc mrk="sxmac.0i.tr">
    <gra><vspec>tr</vspec></gra>
    <dif>
      <tld lit="&Scirc;"/>e man&gcirc;i, kisi:
      <ekz>
        kelkaj el ili melankolie <tld/>is pomojn
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        du bone nutritaj bovinoj, apatie <tld/>antaj fojnon
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i do saltis al lia kolo kaj lin pasie <tld/>adis, balbutante malla&ubreve;te
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_m;&c_o;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_ch;&c_m;&c_ja;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_ch;&c_v;&c_ja;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_h;&c_l;&c_ju;&c_p;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de"><ofc>*</ofc>schmatzen</trd>
  <trdgrp lng="en">
    <trd>smack</trd>,
    <trd><ofc>*</ofc>kiss loudly <klr>(with a smack)</klr></trd>
  </trdgrp>
  <trd lng="es">besar o comer con ruido</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&a_l;&f_ch;&zwnj;&a_m;&a_l;&a_w;&f_ch; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_m;&a_A;&f_ch;&zwnj;&a_w;&a_m;&a_w;&f_ch; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd><ofc>*</ofc>baiser bruyamment</trd>,
    <trd>faire un bruit de <ind>succion</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>cs&aacute;mcsog</trd>,
    <trd>cuppant</trd>,
    <trd>cuppog</trd>
  </trdgrp>
  <trd lng="it">schioccare<klr> (con bocca o labbra)</klr></trd>
  <trd lng="ja">&#12481;&#12517;&#12483;&#12392;&#38899;&#12434;&#31435;&#12390;&#12427;
  <pr>&#12481;&#12517;&#12483;&#12392;&#12362;&#12392;&#12434;&#12383;&#12390;&#12427;</pr></trd>
  <trd lng="nl">smakken</trd>
  <trdgrp lng="pl">
    <trd>cmoka&cacute;</trd>,
    <trd>mlaska&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_a;&c_v;&c_k;&c_a;&c_t;&c_mol;</trd>,
    <trd><ofc>*</ofc>&c_ch;&c_m;&c_o;&c_k;&c_a;&c_t;&c_mol;
<klr>(&c_g;&c_u;&c_b;&c_a;&c_m;&c_i;)</klr></trd>,
    <trd>&c_h;&c_l;&c_ju;&c_p;&c_a;&c_t;&c_mol; <klr>(&c_o;
&c_g;&c_r;&c_ja;&c_z;&c_i;)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ch;&c_m;&c_o;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_l;&c_ja;&c_m;&c_k;&c_a;&c_t;&c_i; (&c_g;&c_u;&c_b;&c_a;&c_m;&c_i;)</trd>,
    <trd>&c_c;&c_m;&c_o;&c_k;&c_a;&c_t;&c_i; (&c_ja;&c_z;&c_i;&c_k;&c_o;&c_m;)</trd>,
    <trd>&c_ch;&c_a;&c_v;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_h;&c_l;&c_ju;&c_p;&c_o;&c_t;&c_ib;&c_t;&c_i; (&c_p;&c_r;&c_o; &c_b;&c_o;&c_l;&c_o;&c_t;&c_o;)</trd>
  </trdgrp>
</drv>
</art>
