<art mrk="$Id: griled.xml,v 1.12 2022/03/19 19:52:44 revo Exp $">
<kap>
  <rad>griled</rad>/oj
</kap>
<drv mrk="griled.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="ortopter.0oj">ortopteroj</ref>
      (<trd lng="la">Gryllidae</trd>), kies maskloj a&ubreve;digas akran
      bruon per la frotado de la anta&ubreve;aj flugiloj:
      <ekz>
        la&ubreve; la le&gcirc;o de <nom>Dolbear</nom> &ccirc;e <tld/>oj oni povas kalkuli la temperaturon kalkulante
        la frotsonojn en certa tempintervalo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_mol;&c_v;&c_ib;&c_r;&c_k;&c_u;&c_n;&c_y; <klr>(&c_s;&c_ja;&c_m;&#x2019;&c_ja;)</klr></trd>
  <trd lng="bg">&c_shch;&c_u;&c_r;&c_c;&c_i;</trd>
  <trd lng="ca">gr&iacute;l&middot;lids</trd>
  <trd lng="da">f&aring;rekyllingen</trd>
  <trd lng="de">Grillen<klr tip="ind">(Familie)</klr></trd>
  <trd lng="en">crickets<klr tip="amb"> (insect)</klr></trd>
  <trd lng="fr">gryllid&eacute;s</trd>
  <trd lng="hr">zrikavci</trd>
  <trd lng="hu">val&oacute;di <ind>t&uuml;csk&ouml;k</ind><klr tip="ind">
  (csal&aacute;d)</klr></trd>
  <trd lng="nl">krekels</trd>
  <trd lng="ru">&c_s;&c_v;&c_je;&c_r;&c_ch;&c_k;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sv">syrsor</trd>
  <trd lng="uk">&#x446;&#x432;&#x456;&#x440;&#x43A;&#x443;&#x43D;&#x43E;&#x432;&#x456;</trd>
</drv>
