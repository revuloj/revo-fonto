<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>tabu</rad>/o
</kap>
<drv mrk="tabu.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Religieca institucio,  kiu igas netu&scirc;eblaj iujn personojn a&ubreve; objektojn a&ubreve; nefareblaj iujn agojn. 
    </dif>
  </snc>
  <snc>
    <dif>
      Malpermeso ion fari a&ubreve; diri pro sociaj kutimoj.
    </dif>
  </snc>
  <snc>
    <dif>
      Afero kiu pro sociaj kutimoj estas malperemsata.
    </dif>
  </snc>
  <trd lng="en">taboo</trd>
  <trd lng="nl">taboe</trd>
  <trd lng="sv">tabu</trd>
</drv>
<drv mrk="tabu.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
    Netu&scirc;ebla a&ubreve; malpermesata pro <tld/>o 
    </dif>
    <refgrp tip="sin">
    <ref cel="sankt.0a">sankta</ref>.
    </refgrp>
  </snc>
</drv>
<drv mrk="tabu.0eco">
  <kap><tld/>eco</kap>
  <snc>
  <dif>
  Teruriga karaktero de personoj a&ubreve; objektoj, kiujn oni ne povas tu&scirc;i sen risko je puno a&ubreve; infekto. 
  </dif>
  <refgrp tip="sin">
  <ref cel="totem.0o">totemo</ref>.
  </refgrp>
  </snc>
</drv>
</art>
