<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>idiom</rad>/o
</kap>
<drv mrk="idiom.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Lingvo propra al nacio a&ubreve; provinco:
      <ekz>
        la <tld/>oj de Centre&ubreve;ropo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dialek.0o">dialekto</ref>,
      <ref cel="jxargo.0o">&jcirc;argono</ref>,
      <ref cel="slang.0o">slango</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Idiom</trd>,
    <trd>Sprache</trd>
  </trdgrp>
  <trd lng="es">idioma</trd>
  <trd lng="fr">idiome</trd>
  <trdgrp lng="nl">
    <trd>taaleigen</trd>,
    <trd>idioom</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_i;&c_a;&c_l;&c_je;&c_k;&c_t;</trd>
</drv>

<drv mrk="idiom.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">LIN</uzo>
  <snc>
    <ref tip="dif" cel="idioti.0o">idiotismo</ref>
  </snc>
  <trdgrp lng="de">
    <trd>idiomatischer Ausdruck</trd>,
    <trd>Spracheigenheit</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>idiotisme</trd>,
    <trd>expression idiomatique</trd>
  </trdgrp>
  <trd lng="nl">idiotisme</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_d;&c_i;&c_o;&c_m;&c_a;</trd>,
    <trd>&c_i;&c_d;&c_i;&c_o;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; &c_v;&c_y;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_f;&c_r;&c_a;&c_z;&c_je;&c_o;&c_l;&c_o;&c_g;&c_i;&c_z;&c_m;</trd>
  </trdgrp>
</drv>
</art>
