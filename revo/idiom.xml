<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>2</ofc>
      <rad>idiom</rad>/o
    </kap>

    <drv mrk="idiom.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">LIN</uzo>
      <snc>
	<dif>
	  &Ccirc;ia ajn pli-malpli kompleta lingvosistemo (homlingvo),
	  supernocio por la nocioj <ctl>norma literatura lingvo</ctl>,
	  <ctl>norma skriba lingvo</ctl>,  <ctl>faka lingvo</ctl>,
	  <refgrp tip="sub">
	    <ref cel="dialek.0o">dialekto</ref>,
	    <ref cel="jxargo.0o">&jcirc;argono</ref>,
	    <ref cel="slang.0o">slango</ref>
	  </refgrp>
	  ktp, uzata precipe kiam oni ne povas a&ubreve; ne bezonas precizigi la
	  statuson a&ubreve; specon de la <tld/>o:
	  <ekz>
	    multajn afrikajn <tld/>ojn oni ne povas facile klasi en
	    lingvojn kaj dialektojn.
	  </ekz>
	</dif>
      </snc>
      <ref tip="sin" cel="lingv.0o.speciala">lingvo<sncref/></ref>
      <trd lng="be">&c_ib;&c_d;&c_y;&c_jo;&c_m;</trd>
      <trdgrp lng="de">
	<trd>Idiom</trd>,
	<trd>Sprache</trd>
      </trdgrp>
      <trd lng="es">idioma</trd>
      <trd lng="fr">idiome</trd>
      <trdgrp lng="hu">
	<trd>idi&oacute;ma</trd>,
	<trd>nyelv</trd>
      </trdgrp>
      <trdgrp lng="nl">
	<trd>taaleigen</trd>,
	<trd>idioom</trd>
      </trdgrp>
      <trd lng="ru">&c_i;&c_d;&c_i;&c_o;&c_m;</trd>
    </drv>

    <drv mrk="idiom.0ajxo">
      <kap><tld/>a&jcirc;o</kap>
      <uzo tip="fak">LIN</uzo>
      <snc>
	<dif>
	  Leksika a&ubreve; frazeologia aparta&jcirc;o de iu <tld/>o, propra al &gcirc;i
	  sed nekutima a&ubreve; e&ccirc; tute nekomprenebla ekster tiu <tld/>o, precipe
	  <ref tip="sub" cel="idioti.0o">idiotismo</ref>.
	</dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_ib;&c_d;&c_y;&c_jo;&c_m;&c_a;</trd>,
        <trd>&c_ib;&c_d;&c_y;&c_ja;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y; &c_v;&c_y;&c_r;&c_a;&c_z;</trd>
      </trdgrp>
      <trdgrp lng="de">
	<trd>idiomatischer Ausdruck</trd>,
	<trd>Spracheigenheit</trd>
      </trdgrp>
      <trdgrp lng="fr">
	<trd>idiotisme</trd>,
	<trd>expression idiomatique</trd>
      </trdgrp>
      <trdgrp lng="hu">
	<trd>idiomatizmus</trd>,
	<trd>nyelvi saj&aacute;toss&aacute;g</trd>
      </trdgrp>
      <trd lng="nl">idiotisme</trd>
      <trdgrp lng="ru">
	<trd>&c_i;&c_d;&c_i;&c_o;&c_m;&c_a;</trd>,
	<trd>&c_i;&c_d;&c_i;&c_o;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; &c_v;&c_y;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>,
	<trd>&c_f;&c_r;&c_a;&c_z;&c_je;&c_o;&c_l;&c_o;&c_g;&c_i;&c_z;&c_m;</trd>
      </trdgrp>
    </drv>
  </art>
