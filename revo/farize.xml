<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>farize</rad>/o <fnt>B</fnt>
</kap>
<drv mrk="farize.0o">
  <kap><tld/>o</kap>
  <snc mrk="farize.0o.HIS">
    <ref tip="dif" cel="farise.0o.HIS">fariseo <sncref/></ref>
    <trd lng="be">&c_f;&c_a;&c_r;&c_y;&c_s;&c_e;&c_j;</trd>
  </snc>
  <snc mrk="farize.0o.FIG">
    <ref tip="dif" cel="farise.0o.FIG">fariseo <sncref/></ref>
    <trdgrp lng="be">
      <trd>&c_f;&c_a;&c_r;&c_y;&c_s;&c_e;&c_j;</trd>,
      <trd>&c_k;&c_r;&c_y;&c_v;&c_a;&c_d;&c_u;&c_sh;&c_n;&c_ib;&c_k;</trd>
    </trdgrp>
  </snc>
</drv>
</art>

