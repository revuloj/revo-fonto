<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>farize</rad>/o <fnt>B</fnt>
</kap>

<drv mrk="farize.0o">
  <kap><tld/>o</kap>
  <snc mrk="farize.0o.HIS">
    <dif>
      <ref tip="dif" cel="farise.0o.HIS">Fariseo<sncref/></ref>:
      <ekz>
        mi ne devenas de la granduloj, mi ne apartenas al la pastra
        ordeno, mi ne estas <tld/>o
        <fnt>
        <aut>Laszlo Balassy, trad. J. Hamwai</aut>,
        <vrk>Jozefo, la &ccirc;arpentisto</vrk>,
        <lok><url
        ref="http://www.b-eichkorn.homepage.t-online.de/dibe45.htm">Dio
        benu, 2002-12</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_f;&c_a;&c_r;&c_y;&c_s;&c_e;&c_j;</trd>
    <trd lng="de">Pharis&auml;er</trd>
    <trd lng="en">Pharisee</trd>
    <trd lng="hu">farizeus</trd>
    <trd lng="nl">Farizee&euml;n</trd>
    <trd lng="ru">&c_f;&c_a;&c_r;&c_i;&c_s;&c_je;&c_j;</trd>
  </snc>
  <snc mrk="farize.0o.FIG">
    <dif>
      <ref tip="dif" cel="farise.0o.FIG">Fariseo<sncref/></ref>:
      <ekz>
        vi <klr>[...]</klr> la rolon ludas kun cinik' de <tld/>o en la templo, kaj des pli mi malamas vin <klr>[...]</klr>
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk>La Stranga Butiko</vrk>
          <lok>Kremfakul'!</lok>
        </fnt>;
      </ekz>
      <ekz>
        Dio &scirc;angis adreson: dira&jcirc;o de liberita <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref='https://eo.wikipedia.org/?curid=111604'>Odon Vallet</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_f;&c_a;&c_r;&c_y;&c_s;&c_e;&c_j;</trd>,
      <trd>&c_k;&c_r;&c_y;&c_v;&c_a;&c_d;&c_u;&c_sh;&c_n;&c_ib;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Pharis&auml;er</trd>,
      <trd>Scheinheiliger</trd>,
      <trd>Heuchler</trd>
    </trdgrp>
    <trd lng="en">pharisee</trd>
    <trd lng="es">fariseo</trd>
    <trdgrp lng="hu">
      <trd>farizeus</trd>,
      <trd>k&eacute;pmutat&oacute;</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>huichelaar</trd>,
      <trd>schijnheilige</trd>,
      <trd>farizee&euml;r</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_f;&c_a;&c_r;&c_i;&c_s;&c_je;&c_j;</trd>,
      <trd>&c_l;&c_i;&c_c;&c_je;&c_m;&c_je;&c_r;</trd>,
      <trd>&c_h;&c_a;&c_n;&c_zh;&c_a;</trd>
    </trdgrp>
  </snc>
  <trd lng="ca">fariseu</trd>
  <trdgrp lng="cs">
    <trd>farizej</trd>,
    <trd>farizeus</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>pharisien</trd>,
    <trd>tartuffe</trd>
  </trdgrp>
  <trd lng="pt">fariseu</trd>
  <trd lng="sk">farizej</trd>
</drv>
</art>
