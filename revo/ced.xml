<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ced</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ced.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc mrk="ced.0i.malrezisti">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Ne kontra&ubreve;stari:
      <ekz>
        fine la malamika armeo <tld/>is;
      </ekz>
      <ekz>
        al tiu &ccirc;io <tld/>as, kiu monon
        posedas<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        hakilo estas tran&ccirc;a, sed ne <tld/>as la
        bran&ccirc;o<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la rutino fine <tld/>is al la sana prudento;
      </ekz>
      <ekz>
        <tld/>i al nacia kutimo, al siaj pasioj, al la
        admonoj, al peto<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la faraono <tld/>is al la deziro de sia fidela servisto
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 23a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i al la kaprico de iu<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i al via insistado<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la febro <tld/>is;
      </ekz>
      <ekz>
        la nokto <tld/>is al griza vintra tagi&gcirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        anta&ubreve; ol la tago <tld/>os al la nokto
        <fnt><bib>MT</bib>, <lok>&Jes; 38:13</lok></fnt>;
      </ekz>
      <ekz>
        la sento de persona indeco ne <tld/>is en &scirc;i
        anta&ubreve; la timo de mizero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi <tld/>as, ke vi estas prava.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fleks.0igxi">fleksi&gcirc;i</ref>,
      <ref cel="klin.0igxi">klini&gcirc;i</ref>,
      <ref cel="obe.0i">obei</ref>,
      <ref cel="sub">subi&gcirc;i</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>nachgeben</trd>, <trd>weichen</trd>
    </trdgrp>
    <trd lng="hu">enged</trd>
    <trd lng="nl">toelaten</trd>
    <trd lng="pl">ust&eogonek;powa&cacute;</trd>
  </snc>
  <snc mrk="ced.0i.fordoni">
    <gra><vspec>tr</vspec></gra>
    <dif>
      Lasi al iu ion, kion oni okupis, posedis a&ubreve; pretendis:
      <ekz>
        <tld/>i sian lokon, se&gcirc;on<fnt>B</fnt>,
        la tronon, siajn rajtojn;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la espero <tld/>is lokon al plena disrevi&gcirc;o;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;ia anta&ubreve;a paleco <tld/>is la lokon al flama
        ru&gcirc;o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="las.al0i">allasi</ref>,
      <ref cel="okup.mal0i">malokupi</ref>,
      <ref cel="tir">sin retiri</ref>,
      <ref cel="rezign.0i">rezigni</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>abtreten</trd>, <trd>abgeben</trd>,
      <trd>&uuml;berlassen</trd>
    </trdgrp>
    <trd lng="hu">&aacute;tenged</trd>
    <trd lng="nl">afdanken</trd>
    <trdgrp lng="pl">
      <trd>cedowa&cacute;</trd>,
      <trd>oddawa&cacute;</trd>,
      <trd>ust&eogonek;powa&cacute;</trd>,
      <trd>zrzeka&cacute; si&eogonek;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_s;&c_t;&c_u;&c_p;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_d;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">cedir</trd>
  <trd lng="en">cede</trd>
  <trd lng="es">ceder</trd>
  <trdgrp lng="fr">
    <trd>c&eacute;der</trd>,
    <trd>conc&eacute;der</trd>
  </trdgrp>
  <trd lng="it">cedere</trd>
  <trdgrp lng="pt">
    <trd>ceder</trd>,
    <trd>conceder</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_t;&c_u;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_s;&c_t;&c_u;&c_p;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_d;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="ced.0o">
  <kap><tld/>o</kap>
  <snc mrk="ced.0o.KOMUNE">
    <dif>
      Ago de tiu, kiu <tld/>as:
      <ekz>
        mi ne povas fari pliajn <tld/>ojn;
      </ekz>
      <ekz>
        disciplina <tld/>o de la malplimulto al la
        plimulto<fnt>Z</fnt>;
      </ekz>
      <ekz>
        oni povas ripro&ccirc;i al tiu traduko tro da <tld/>o al la
        apartaj kutimoj de la rusa lingvo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_s;&c_t;&c_u;&c_p;&c_k;&c_a; <klr
  tip="amb">(&c_d;&c_z;&c_je;&c_ja;&c_n;&c_mol;&c_n;&c_je;)</klr></trd>
  <trd lng="ca">cessi&oacute;</trd>
    <trdgrp lng="de">
      <trd>Abtretung</trd>, <trd>&Uuml;berlassung</trd>
    </trdgrp>
  <trd lng="es">concesi&oacute;n</trd>
  <trd lng="fr">concession <klr tip="ind">(fait de
  c&eacute;der)</klr></trd>
  <trd lng="hu">enged&eacute;s</trd>
  <trd lng="it">concessione</trd>
  <trdgrp lng="pl">
    <trd>ust&eogonek;pstwo</trd>,
    <trd>zrzeczenie si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>concess&atilde;o</trd>,
    <trd>cess&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_s;&c_t;&c_u;&c_p;&c_k;&c_a;</trd>
</drv>

<drv mrk="ced.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="ced.0ajxo.KOMUNE">
    <dif>
      Tio, kion oni <tld/>as.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_s;&c_t;&c_u;&c_p;&c_k;&c_a; <klr
  tip="amb">(&c_n;&c_je;&c_sh;&c_t;&c_a;
  &c_s;&c_a;&c_s;&c_t;&c_u;&c_p;&c_l;&c_je;&c_n;&c_a;&c_je;)</klr></trd>
  <trd lng="ca">all&ograve; cedit</trd>
  <trdgrp lng="de">
    <trd>Abgetretenes</trd>,
    <trd>&Uuml;berlassenes</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cession</trd>,
    <trd>concession</trd>
  </trdgrp>
  <trd lng="hu">engedm&eacute;ny</trd>
  <trd lng="it">concessione</trd>
  <trdgrp lng="pl">
    <trd>odst&eogonek;pne</trd>,
    <trd>ust&eogonek;pstwo</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_s;&c_t;&c_u;&c_p;&c_k;&c_a;<klr> (&c_t;&c_o;,
  &c_ch;&c_t;&c_o;
  &c_u;&c_s;&c_t;&c_u;&c_p;&c_l;&c_je;&c_n;&c_o;)</klr></trd>
</drv>

<drv mrk="ced.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="ced.0ema.KOMUNE">
    <dif>
      Inklina <tld/>i, akordi&gcirc;ema.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_g;&c_o;&c_d;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_ja;&c_r;&c_k;&c_o;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_a;&c_t;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>condescendent</trd>,
    <trd>d&ograve;cil</trd>,
    <trd>permissiu</trd>
  </trdgrp>
  <trd lng="de">nachgiebig</trd>
  <trdgrp lng="fr">
    <trd>conciliant</trd>,
    <trd>docile</trd>
  </trdgrp>
  <trd lng="hu">enged&eacute;keny</trd>
  <trdgrp lng="it">
   <trd>cedevole</trd>, 
   <trd>accondiscendente</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>toegeeflijk</trd>,
    <trd>onstandvastig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ust&eogonek;pliwy</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_t;&c_u;&c_p;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_a;&c_t;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="ced.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ced.for0i.KOMUNE">
    <dif>
      Senreziste fordoni:
      <ekz>
        la a&ubreve;toro por &ccirc;iam for<tld/>as &ccirc;iujn personajn rajtojn
        <fnt>
          <aut>L. L. Zamenhof</aut>,
          <vrk>Unua Libro</vrk>,
          <lok>1887</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">conc&eacute;der</trd>
  <trdgrp lng="nl">
    <trd>afstaan</trd>,
    <trd>afstand doen van</trd>
  </trdgrp>
</drv>

<drv mrk="ced.mal0i">
  <kap>mal<tld/>i</kap>
  <snc mrk="ced.mal0i.KOMUNE">
    <dif>
      Rifuze kontra&ubreve;stari, rezisti.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol;
    <ind>&c_n;&c_ja;&c_w;&c_s;&c_t;&c_u;&c_p;&c_l;&c_ib;&c_v;&c_y;&c_m;</ind></trd>,
    <trd>&c_s;&c_u;&c_p;&c_r;&c_a;&c_c;&c_ib;&c_w;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_u;&c_p;&c_a;&c_r;&c_c;&c_ib;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>entestar-se</trd>,
    <trd>enderiar-se</trd>
  </trdgrp>
    <trdgrp lng="de">
      <trd>widerstehen</trd>,
      <trd><klr>sich </klr>behaupten</trd>
    </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>obstiner</ind></trd>,
    <trd>rester <ind>ferme</ind></trd>,
    <trd>r&eacute;sister</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>dacol</trd>,
    <trd>makacskodik</trd>,
    <trd>ellen&aacute;ll</trd>
  </trdgrp>
  <trdgrp lng="it">
   <trd>resistere</trd>, 
   <trd>reggere</trd>
  </trdgrp>
  <trdgrp lng="nl">
   <trd>weerstaan</trd>, 
   <trd>weerstand bieden</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przeciwstawia&cacute; si&eogonek;</trd>,
    <trd>oponowa&cacute;</trd>,
    <trd>by&cacute; <ind>nieust&eogonek;pliwym</ind></trd>,
    <trd>okazywa&cacute;
    <ind>nieust&eogonek;pliwo&sacute;&cacute;</ind></trd>,
    <trd>by&cacute; <ind>nieugi&eogonek;tym</ind></trd>,
    <trd>okazywa&cacute;
    <ind>nieugi&eogonek;to&sacute;&cacute;</ind></trd>,
    <trd>trwa&cacute;</trd>,
    <trd>wytrzymywa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_ja;&c_v;&c_l;&c_ja;&c_t;&c_mol;
    <ind>&c_n;&c_je;&c_u;&c_s;&c_t;&c_u;&c_p;&c_ch;&c_i;&c_v;&c_o;&c_s;&c_t;&c_mol;</ind></trd>,
    <trd>&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="ced.mal0ema">
  <kap>mal<tld/>ema</kap>
  <snc mrk="ced.mal0ema.KOMUNE">
    <dif>
      Esti kontra&ubreve;staranta, rezistema.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_z;&c_g;&c_o;&c_d;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_w;&c_s;&c_t;&c_u;&c_p;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>testarrut</trd>,
    <trd>obstinat</trd>,
    <trd>acerat</trd>
  </trdgrp>
    <trdgrp lng="de">
      <trd>standhaft</trd>,
      <trd>unbeugsam</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>but&eacute;<klr> (ent&ecirc;t&eacute;)</klr></trd>,
    <trd>ent&ecirc;t&eacute;</trd>
  </trdgrp>
  <trdgrp lng="it">
   <trd>resistente</trd>, 
   <trd>intransigente</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>standvastig</trd>,
    <trd>onverzettelijk</trd>,
    <trd>onbuigzaam</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>nieust&eogonek;pliwy</trd>,
    <trd>nieugi&eogonek;ty</trd>
  </trdgrp>
  <trd
  lng="ru">&c_n;&c_je;&c_u;&c_s;&c_t;&c_u;&c_p;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>
</drv>

</art>
