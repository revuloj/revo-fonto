  <snc mrk="ced.mal0ema.KOMUNE">
    <dif>
      Esti kontra&ubreve;staranta, rezistema:
      <ekz>
        Dio obstinigis lian spiriton kaj mal<tld/>emigis lian koron
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 2:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        la obstinkapa, la mal<tld/>ema homo
        <fnt>
          <aut>Centro pri Biblia Studado G. Vannucci</aut>,
          <vrk><url ref="http://www.studibiblici.it/esperanto/Omelie/07_02_10rev.pdf">
          Ili forlasis &ccirc;ion kaj sekvis lin</url></vrk>,
          <lok>2010-02-07</lok>
        </fnt>;
      </ekz>
      <ekz>
        religia mal<tld/>emo
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="https://www.monato.be/2013/011081.php">Astrobiologio kaj
          religio</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
       <ref cel="obstin.0a">obstina</ref>,
       <ref cel="spit.0ema">spitema</ref>
