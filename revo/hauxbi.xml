<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hauxbi.xml,v 1.17 2017/09/12 18:10:20 revo Exp $">
<kap>
  <rad>ha&ubreve;biz</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="hauxbi.0o">
  <kap><tld/>o</kap>
  <snc mrk="hauxbi.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="super" cel="paf.0ilego">Pafilego</ref> kies tubo
      estas 20&ndash;30 kalibrojn longa (do, malpli longa ol tiu de
      <ref cel="kanon1.0o.pafilo">kanono<sncref/></ref>),
      destinita por pafi la&ubreve; granda celangulo trans
      protektajn &scirc;irmilojn (trans montetojn ktp):
      <ekz>
        ili subite a&ubreve;dis serion da eksplodoj, kaj je la krio
        &leftquot;<tld lit="H"/>oj,&rightquot; &jcirc;etis sin teren
        <fnt>
          <aut>J. Francis</aut>
          <vrk>La Granda Kaldrono</vrk>
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_w;&c_b;&c_ib;&c_c;&c_a;</trd>
  <trd lng="bg">&c_g;&c_a;&c_u;&c_b;&c_i;&c_c;&c_a;</trd>
  <trd lng="de">Haubitze</trd>
  <trd lng="es"><klr>ca&ntilde;&oacute;n</klr> ob&uacute;s</trd> 
  <trd lng="fr">obusier</trd>
  <trdgrp lng="hu">
    <trd>tarack</trd>,
    <trd>mozs&aacute;r&aacute;gy&uacute;</trd>,
    <trd>haubic</trd>,
  </trdgrp>
  <trd lng="it">obice</trd>
  <trd lng="nl">houwitser</trd>
  <trd lng="pl">haubica</trd>
  <trdgrp lng="pt">
    <trd>obus</trd>,
    <trd>obuseiro</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_u;&c_b;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_t;&c_i;&c_r;&c_a;</trd>
  </trdgrp>
</drv>
</art>
