<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hauxbi.xml,v 1.22 2023/11/15 16:45:18 revo Exp $">
<kap>
  <rad>ha&ubreve;biz</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="hauxbi.0o">
  <kap><tld/>o</kap>
  <snc mrk="hauxbi.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="super" cel="paf.0ilego">Pafilego</ref> kies tubo
      estas 20&ndash;30 kalibrojn longa (do, malpli longa ol tiu de
      <ref cel="kanon1.0o.pafilo">kanono<sncref/></ref>),
      kapabla pafi obusojn la&ubreve; alta kaj 
      malalta trajektorioj:
      <ekz>
        ili subite a&ubreve;dis serion da eksplodoj, kaj je la krio
        &leftquot;<tld lit="H"/>oj,&rightquot; &jcirc;etis sin teren
        <fnt>
          <aut>J. Francis</aut>
          <vrk>La Granda Kaldrono</vrk>
          <lok>1978</lok>
        </fnt>;
      </ekz>
      <ekz>
        Slovakio riparos <tld/>ojn, tankojn kaj kanonojn de siaj polaj partneroj
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2015/011547.php">Armitaj amikoj</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        da&ubreve;re misfunkcias britaj <tld/>oj, kiuj voris jam en la pasinta jaro
        trionmilionon da e&ubreve;roj simple &ndash; kaj vane &ndash; riparocele
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2003/008086.html">Taglibro de l' (anta&ubreve;)milito ... el la lando de &Scirc;ekspiro (2)</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bomb.0opafilo">bombopafilo</ref>,
      <ref cel="kanon1.0o.pafilo">kanono</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_w;&c_b;&c_ib;&c_c;&c_a;</trd>
  <trd lng="bg">&c_g;&c_a;&c_u;&c_b;&c_i;&c_c;&c_a;</trd>
  <trd lng="de">Haubitze</trd>
  <trd lng="es"><klr>ca&ntilde;&oacute;n</klr> ob&uacute;s</trd> 
  <trd lng="fr">obusier</trd>
  <trdgrp lng="hu">
    <trd>tarack</trd>,
    <trd>mozs&aacute;r&aacute;gy&uacute;</trd>,
    <trd>haubic</trd>,
  </trdgrp>
  <trd lng="it">obice</trd>
  <trd lng="nl">houwitser</trd>
  <trd lng="pl">haubica</trd>
  <trdgrp lng="pt">
    <trd>obus</trd>,
    <trd>obuseiro</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_u;&c_b;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_t;&c_i;&c_r;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_a;&c_u;&c_b;&c_i;&c_c;&c_ja;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_t;&c_i;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#x69B4;&#x5F39;&#x70AE; <pr>li&#xFA;d&#xE0;np&#xE0;o</pr></trd>
</drv>
</art>
