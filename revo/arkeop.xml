<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: arkeop.xml,v 1.25 2023/10/28 08:33:08 revo Exp $">
<kap>
  <rad>arkeopterik</rad>/o <fnt><bib>PV</bib></fnt>,
  <var><kap><rad var="g">arkeopterig</rad>/o <fnt><bib>PIV2</bib></fnt></kap></var>
</kap>

<drv mrk="arkeop.0o">
  <kap><tld/>o,
    <var><kap><tld var="g"/>o</kap></var>
  </kap>
  <snc mrk="arkeop.0o.ZOO">
    <uzo tip="fak">PAL</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Transira fosilia besto, <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">genro</ref>
      de prasa&ubreve;roj (<trd lng="la">Archaeopteryx</trd>) la&ubreve;forme inter teropodaj <ref cel="dinosa.0oj">dinosa&ubreve;roj</ref>
      kaj <ref cel="bird.0oj">birdoj</ref>:
      <ekz>
        fosilio de la mondvaste konata &leftquot;pra-birdo&#x201D; <tld var="g"/>o trovi&gcirc;as en la Londona
        Naturhistoria Muzeo (fondita el parto de la Brita Muzeo), kiu &gcirc;in a&ccirc;etis tuj
        post &gcirc;ia malkovro en 1861
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2008/009438.php">Fosilioj en suda Germanio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        proksime de <nom>Kelheim</nom>, <klr>[&#x2026;]</klr> trovi&gcirc;is la &gcirc;is nun plej bone konservita
        fosilio de dinosa&ubreve;ro en E&ubreve;ropo <klr>[&#x2026;]</klr> sciencistoj taksas, ke &gcirc;i estas
        la plej grava fosilio de prasa&ubreve;ro (arkesa&ubreve;ro) trovita en Germanio post la
        malkovro de la konata prabirdo <tld var="g"/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2012/010572.php">Novaj fosilioj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/4/45/Archaeopteryx_lithographica_paris.JPG/451px-Archaeopteryx_lithographica_paris.JPG" prm="PD">litografio en Franca Nacia Muzeo de Naturhistorio de originala <tld/>fosilio en
        Londona Naturhistoria Muzeo
        <fnt>
          <aut>LadyofHats</aut>
          <lok><url ref="&WCW;/File:Archaeopteryx_lithographica_paris.JPG">&WCW;/File:Archaeopteryx_lithographica_paris.JPG</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="zh">
    <trd>&#x59CB;&#x7956;&#x9CE5; <pr>sh&#x1D0;z&#x1D4;ni&#x1CE;o</pr></trd>,
    <trd>&#x59CB;&#x7956;&#x9E1F; <pr>sh&#x1D0;z&#x1D4;ni&#x1CE;o</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_r;&c_h;&c_je;&c_a;&c_p;&c_t;&c_e;&c_r;&c_y;&c_k;&c_s;</trd>
  <trd lng="ca">arque&ograve;pterix</trd>
  <trd lng="cs">archeopteryx</trd>
  <trd lng="de">Archaeopteryx</trd>
  <trd lng="en">archeopteryx</trd>
  <trd lng="es">arqueopteryx</trd>
  <trd lng="fr">arch&eacute;opt&eacute;ryx</trd>
  <trd lng="hu">archeopteryx</trd>
  <trd lng="it">archeopteryx</trd>
  <trd lng="ja">&#x59CB;&#x7956;&#x9CE5;  <pr>&#x3057;&#x305D;&#x3061;&#x3087;&#x3046;</pr></trd>
  <trd lng="ru">&c_a;&c_r;&c_h;&c_je;&c_o;&c_p;&c_t;&c_je;&c_r;&c_i;&c_k;&c_s;</trd>
  <trd lng="sk">archeopteryx</trd>
  <trd lng="uk">&#x430;&#x440;&#x445;&#x435;&#x43E;&#x43F;&#x442;&#x435;&#x440;&#x438;&#x43A;&#x441;</trd>
</drv>
</art>
