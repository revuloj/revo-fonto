<drv mrk="mizogi.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Homo, kiu malamas, evitadas virinojn a&ubreve; havas malfavorajn opiniojn
      pri ili:
      <ekz>
        belaj kaj bonaj viroj estas <tld/>oj
        <fnt>
          <aut>Jevgenij Gaus</aut>,
          <vrk>Le&gcirc;o de Murphi por la virinoj...</vrk>,
          <lok><url ref="http://groups.yahoo.com/neo/groups/esperanto-lt/conversations/topics/179">esperanto-lt</url>,
          2001-08-19</lok>
        </fnt>;
      </ekz>
      <ekz>
        plej trafe kontra&ubreve; rotojn da bu&ccirc;aj <tld/>oj krimantaj por
        kreinto a&ubreve; libro unusola
        <fnt>
          <aut>Jorge Camacho</aut>,
          <vrk>Strangaj spikoj</vrk>,
          <lok><url ref="http://books.google.com/books?id=nWu7CwAAQBAJ">Google
          Books</url>, 2016-03-13</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ib;&c_z;&c_a;&c_g;&c_ib;&c_n;&c_ib;&c_s;&c_t;</trd>,
    <trd>&c_zh;&c_a;&c_n;&c_ch;&c_y;&c_n;&c_a;&c_n;&c_je;&c_n;&c_a;&c_v;&c_ib;&c_s;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">misogin</trd>
  <trd lng="da">kvindehader</trd>
  <trdgrp lng="de">
    <trd>Frauenhasser</trd>,
    <trd>Frauenhasserin</trd>,
    <trd>Frauenfeind</trd>,
    <trd>Frauenfeindin</trd>,
    <trd>Misogyn</trd>
  </trdgrp>
  <trd lng="el">&my;&jota;&sigma;&omikron;&gamma;&ypsilon_ton;&ny;&eta;&sigma_fina;</trd>
  <trd lng="en">misogynist</trd>
  <trdgrp lng="es">
    <trd>mis&oacute;gino</trd>,
    <trd>mis&oacute;gina</trd>
  </trdgrp>
  <trd lng="et">naistevihkaja</trd>
  <trdgrp lng="fi">
    <trd>naistenvihaaja</trd>,
    <trd>misogyyni</trd>
  </trdgrp>
  <trd lng="fr">misogyne</trd>
  <trd lng="ga">fuathaitheoir ban</trd>
  <trdgrp lng="gl">
    <trd>mis&oacute;xino</trd>,
    <trd>mis&oacute;xina</trd>
  </trdgrp>
  <trd lng="hu">n&odblac;gy&udblac;l&ouml;l&odblac;</trd>
  <trd lng="is">kvenhatari</trd>
  <trd lng="mk">&c_zh;&c_je;&c_n;&c_o;&c_m;&c_r;&c_a;&c_z;&c_je;&c_c;</trd>
  <trd lng="nl">vrouwenhater</trd>
  <trd lng="pl">mizoginia</trd>
  <trdgrp lng="pt">
    <trd>mis&oacute;gino</trd>,
    <trd>mis&oacute;gina</trd>
  </trdgrp>
  <trd lng="ro">misoginie</trd>
  <trdgrp lng="ru">
