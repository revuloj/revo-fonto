<drv mrk="mizogi.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Stato de <tld/>o:
      <ekz>
        estus mojose se ni povus preni seksperforton, seksan atakon kaj
        strukturan <tld/>econ &ccirc;iutage tiom serioze kiom ni faras kiam
        migrantoj kaj islamanoj estas implikitaj kiel kulpuloj
        <fnt>
          <vrk>Post Kolonjo, ni ne povas lasi ke la bigotoj &scirc;telu la
          feminismon.</vrk>,
          <lok><url ref="http://egalecen.org/2016/01/19/post-kolonjo-ni-ne-povas-lasi-ke-la-bigotoj-stelu-la-feminismon/">Egalecen</url>,
          2016-01-19</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ib;&c_z;&c_a;&c_g;&c_ib;&c_n;&c_ib;&c_ja;</trd>,
    <trd>&c_zh;&c_a;&c_n;&c_ch;&c_y;&c_n;&c_a;&c_n;&c_je;&c_n;&c_a;&c_v;&c_ib;&c_s;&c_mol;&c_n;&c_ib;&c_c;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">misog&iacute;nia</trd>
