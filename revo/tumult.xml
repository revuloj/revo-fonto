<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tumult</rad>/o
</kap>
<drv mrk="tumult.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Konfuza malorda kaj brua malkvieti&gcirc;o de homamaso:
      <ekz>
        infera <tld/>o<fnt>B</fnt>;
      </ekz>
      <ekz>
        bolis terura <tld/>o<fnt>B</fnt>;
      </ekz>
      <ekz>
        petolema <tld/>o de maskoj;
      </ekz>
      <ekz>
        <tld/>o kunveno;
      </ekz>
      <ekz>
        <tld/>emaj infanoj.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="ekscit">ekscito</ref>,
      <ref cel="agit">agiti&gcirc;o</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Tumult</trd>
  <trd lng="en">riot</trd>
  <trdgrp lng="nl">
    <trd>tumult</trd>,
    <trd>opschudding</trd>,
    <trd>herrie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_m;&c_a;&c_t;&c_o;&c_h;&c_a;</trd>,
    <trd>&c_s;&c_m;&c_ja;&c_t;&c_je;&c_n;&c_i;&c_je; <klr tip="amb">(&c_v; &c_t;&c_o;&c_l;&c_p;&c_je;)</klr></trd>,
    <trd>&c_t;&c_o;&c_l;&c_ch;&c_je;&c_ja;</trd>,
    <trd>&c_d;&c_a;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="tumult.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Brue kaj malorde malkvieti&gcirc;i:
      <ekz>
        kial <tld/>as popoloj kaj gentoj pripensas vana&jcirc;on<fnt>Z</fnt>?
      </ekz>
      <ekz>
        la popol' <tld/>as en supozoj pri l' morto de Polonio<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiam <tld/>i&gcirc;is la tuta komunumo kaj ekmurmuris kontra&ubreve;
        Moseo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">toben</trd>
  <trd lng="en">riot</trd>
  <trd lng="nl">opschudding veroorzaken</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_j;&c_t;&c_i; &c_v; &c_s;&c_m;&c_ja;&c_t;&c_je;&c_n;&c_i;&c_je;<klr tip="ind"> (&c_o; &c_t;&c_o;&c_l;&c_p;&c_je;, &c_n;&c_a;&c_r;&c_o;&c_d;&c_je;)</klr></trd>,
    <trd>&c_v;&c_z;&c_v;&c_o;&c_l;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja; <klr tip="amb">(&c_o; &c_t;&c_o;&c_l;&c_p;&c_je;, &c_n;&c_a;&c_r;&c_o;&c_d;&c_je;)</klr></trd>
  </trdgrp>
</drv>
</art>
