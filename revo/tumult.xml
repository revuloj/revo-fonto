<drv mrk="tumult.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="tumult.0i.KOMUNE">
    <dif>
      Brue kaj malorde malkvieti&gcirc;i:
      <ekz>
        kial <tld/>as popoloj kaj gentoj pripensas
        vana&jcirc;on
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 4:25</lok>
        </fnt>
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 2:1</lok>
        </fnt>?
      </ekz>
      <ekz>
        la popol' <tld/>as en supozoj pri l' morto de
        Polonio
        <fnt>
          <aut>William Shakespeare, trad. L.L. Zmaenhof</aut>
          <vrk><url ref="https://eo.wikisource.org/wiki/Hamleto,_re%C4%9Dido_de_Danujo/Akto_IV">Hamleto,
          re&gcirc;ido de Danujo</url></vrk>
          <lok>Akto IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiam <tld/>i&gcirc;is la tuta komunumo kaj ekmurmuris
        kontra&ubreve; Moseo
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 14:1</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_u;&c_n;&c_t;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_u;&c_z;&c_d;&c_y;&c_m;&c_a;&c_c;&c_mol; <ind>&c_v;&c_e;&c_r;&c_h;&c_a;&c_l;</ind></trd>
  </trdgrp>
  <trd lng="bg">&c_b;&c_u;&c_n;&c_t;&c_u;&c_v;&c_a;&c_m; &c_s;&c_je;</trd>
  <trdgrp lng="ca">
    <trd>avalotar</trd>,
    <trd>revoltar-se</trd>
  </trdgrp>
  <trd lng="cs">hlu&ccaron;et</trd>
