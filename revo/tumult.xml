    <trd>se <ind>soulever</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tolong</trd>,
    <trd>t&uuml;lekedik</trd>,
    <trd>kavarog</trd>,
    <trd>zavago</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x9A12;&#x52D5;&#x3059;&#x308B;  <pr>&#x305D;&#x3046;&#x3069;&#x3046;&#x3059;&#x308B;</pr></trd>,
