<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tumult.xml,v 1.20 2017/11/03 15:10:13 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tumult</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tumult.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tumult.0o.KOMUNE">
    <dif>
      Konfuza, malorda bruado kaj malkvieti&gcirc;o de homamaso,
      eventuale kun eksciti&gcirc;o kaj perfortoj:
      <ekz>
        kiam vi a&ubreve;dos pri militoj kaj <tld/>oj, ne teruri&gcirc;u
        <fnt>
          <bib>NT</bib>
          <lok>Luko 21:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        fari&gcirc;os <tld/>o en via popolo, kaj &ccirc;iuj viaj fortika&jcirc;oj estos
        detruitaj
        <fnt>
          <bib>MT</bib>
          <lok>Ho&scirc;ea 10:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        infera <tld/>o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        bolis terura <tld/>o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o <klr>[de]</klr> karnavala parado
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la <tld/>o de la batalo
        <fnt>
          <bib>Fab3</bib>
          <lok>Dorna vojo de la honoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la agema <tld/>o de la &ccirc;iutaga vivo
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="agit">agiti&gcirc;o</ref>,
      <ref cel="pusx.inter0igxo.MOV">interpu&scirc;i&gcirc;o</ref>,
      <ref cel="ribel.0o.POL">ribelo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_b;&c_u;&c_n;&c_t;</trd>,
    <trd>&c_s;&c_u;&c_m;&c_a;&c_t;&c_o;&c_h;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>tumult</trd>,
    <trd>avalot</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mela</trd>,
    <trd>n&aacute;val</trd>,
    <trd>povyk</trd>,
    <trd>rej</trd>,
    <trd>rozruch</trd>,
    <trd>ruch</trd>,
    <trd>shluk</trd>,
    <trd>shon</trd>,
    <trd>srocen&iacute;</trd>,
    <trd>tla&ccaron;enice</trd>,
    <trd>tumult</trd>,
    <trd>v&rcaron;ava</trd>,
    <trd>zmatek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Tumult</trd>,
    <trd>Unruhe</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>tumult</trd>,
    <trd>riot</trd>,
    <trd>commotion</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>tumulto</trd>,
    <trd>disturbio</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;meute</trd>,
    <trd>tumulte</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tolong&aacute;s</trd>,
    <trd>t&uuml;leked&eacute;s</trd>,
    <trd>kavarod&aacute;s</trd>,
    <trd>tumultus</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>tumult</trd>,
    <trd>opschudding</trd>,
    <trd>herrie</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>tumult</trd>,
    <trd>harmider</trd>,
    <trd>rwetest</trd>,
    <trd>wrzawa</trd>,
    <trd>zamieszanie</trd>,
    <trd>zgie&lstroke;k</trd>
  </trdgrp>
  <trd lng="pt">tumulto</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_m;&c_a;&c_t;&c_o;&c_h;&c_a;</trd>,
    <trd>&c_s;&c_m;&c_ja;&c_t;&c_je;&c_n;&c_i;&c_je; <klr tip="amb">(&c_v; &c_t;&c_o;&c_l;&c_p;&c_je;)</klr></trd>,
    <trd>&c_t;&c_o;&c_l;&c_ch;&c_je;&c_ja;</trd>,
    <trd>&c_d;&c_a;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hurhaj</trd>,
    <trd>trma-vrma</trd>,
    <trd>zhluk</trd>,
    <trd>zhon</trd>,
    <trd>zm&auml;tok</trd>
  </trdgrp>
</drv>

<drv mrk="tumult.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="tumult.0i.KOMUNE">
    <dif>
      Brue kaj malorde malkvieti&gcirc;i:
      <ekz>
        kial <tld/>as popoloj kaj gentoj pripensas
        vana&jcirc;on
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 4:25</lok>
        </fnt>
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 2:1</lok>
        </fnt>?
      </ekz>
      <ekz>
        la popol' <tld/>as en supozoj pri l' morto de
        Polonio
        <fnt>
          <aut>William Shakespeare, trad. L.L. Zmaenhof</aut>
          <vrk><url
          ref="https://eo.wikisource.org/wiki/Hamleto,_re%C4%9Dido_de_Danujo/Akto_IV">Hamleto,
          re&gcirc;ido de Danujo</url></vrk>
          <lok>Akto IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiam <tld/>i&gcirc;is la tuta komunumo kaj ekmurmuris
        kontra&ubreve; Moseo
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 14:1</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_b;&c_u;&c_n;&c_t;&c_u;&c_v;&c_a;&c_m; &c_s;&c_je;</trd>
  <trdgrp lng="ca">
    <trd>avalotar</trd>,
    <trd>revoltar-se</trd>
  </trdgrp>
  <trd lng="cs">hlu&ccaron;et</trd>
  <trd lng="de">toben</trd>
  <trdgrp lng="en">
    <trd>rage</trd>,
    <trd>riot</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>tumultuar</trd>,
    <trd>alborotar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>agiter</ind></trd>,
    <trd>chahuter</trd>,
    <trd>se <ind>soulever</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tolong</trd>,
    <trd>t&uuml;lekedik</trd>,
    <trd>kavarog</trd>,
    <trd>zavago</trd>
  </trdgrp>
  <trd lng="nl">opschudding veroorzaken</trd>
  <trdgrp lng="pl">
    <trd>robi&cacute; <ind>tumult</ind></trd>,
    <trd>robi&cacute; <ind>harmider</ind></trd>,
    <trd>robi&cacute; <ind>rwetest</ind></trd>,
    <trd>robi&cacute; <ind>wrzaw&eogonek;</ind></trd>,
    <trd>robi&cacute; <ind>zamieszanie</ind></trd>,
    <trd>robi&cacute; <ind>zgie&lstroke;k</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_j;&c_t;&c_i; &c_v;
    &c_s;&c_m;&c_ja;&c_t;&c_je;&c_n;&c_i;&c_je;<klr tip="ind"> (&c_o;
    &c_t;&c_o;&c_l;&c_p;&c_je;,
    &c_n;&c_a;&c_r;&c_o;&c_d;&c_je;)</klr></trd>,
    <trd>&c_v;&c_z;&c_v;&c_o;&c_l;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;
    <klr tip="amb">(&c_o; &c_t;&c_o;&c_l;&c_p;&c_je;,
    &c_n;&c_a;&c_r;&c_o;&c_d;&c_je;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>b&uacute;ri&tcaron; sa</trd>,
    <trd>nepokojne vrie&tcaron; v dave</trd>,
    <trd>pla&scaron;i&tcaron; sa</trd>
  </trdgrp>
</drv>

<drv mrk="tumult.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="tumult.0ema.KOMUNE">
    <dif>
      Kiu volonte <tld/>as, ka&ubreve;zas malkvieton:
      <ekz>
        <tld/>emaj infanoj;
      </ekz>
      <ekz>
        &ccirc;efo de potenca kaj <tld/>ema familio
        <fnt>
          <vrk><url
          ref="https://eo.wikipedia.org/wiki/Aleksandro_la_6-a">Aleksandro
          la 6-a</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_b;&c_u;&c_je;&c_n;</trd>
  <trdgrp lng="fr">
    <trd>agit&eacute;</trd>,
    <trd>chahuteur</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>tumultuoso</trd>,
    <trd>tumultuario</trd>,
    <trd>alborotador</trd>,
    <trd>follonero</trd>
  </trdgrp>
</drv>
</art>
