<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>hezit</rad>/i
</kap>
<drv mrk="hezit.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Pro dubo &scirc;anceli&gcirc;i, halti; ne scii certe, &ccirc;u oni faros
      a&ubreve; diros ion, a&ubreve; ne:
      <ekz>
        <tld/>i inter du decidoj, partioj;
      </ekz>
      <ekz>
        <tld/>i kun respondo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i pri la finado;
      </ekz>
      <ekz>
        <tld/>i deklamante rolon;
      </ekz>
      <ekz>
        mi <tld/>as preni sur min tiun taskon;
      </ekz>
      <ekz>
        tiu rimarko forte <tld/>igis min;
      </ekz>
      <ekz>
        <ind>sen<tld/>e</ind> paroli, deklari;
        <trdgrp lng="de">
           <trd>ohne zu z&ouml;gern</trd>,
           <trd>ohne Z&ouml;gern</trd>,
           <trd>anstandslos</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>sans <ind>h&eacute;sitation</ind></trd>,
          <trd>r&eacute;sol&ucirc;ment</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind><tld/>a</ind>j respondoj, pa&scirc;oj;
        <trdgrp lng="de">
           <trd>z&ouml;gernd</trd>,
           <trd>unschl&uuml;ssig</trd>
        </trdgrp>
        <trd lng="fr">h&eacute;sitant</trd>
      </ekz>
      <ekz>
        <ind><tld/>ema</ind> karaktero.
        <trdgrp lng="de">
           <trd>z&ouml;gerlich</trd>,
           <trd>zaudernd</trd>,
           <trd>unschl&uuml;ssig</trd>
        </trdgrp>
        <trd lng="fr">h&eacute;sitant</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxance.0igxi">&scirc;anceli&gcirc;i</ref>,
      <ref cel="tim.0i">timi</ref>,
      <ref cel="palp.0i">palpi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>z&ouml;gern</trd>,
    <trd>zaudern</trd>
  </trdgrp>
  <trd lng="el">&delta;&jota;&sigma;&tau;&alfa_ton;&zeta;&omega;</trd>
  <trd lng="en">hesitate</trd>
  <trdgrp lng="es">
    <trd>titubear</trd>,
    <trd>vacilar</trd>
  </trdgrp>
  <trd lng="fr">h&eacute;siter</trd>
  <trd lng="nl">aarzelen</trd>
  <trd lng="pl">waha&cacute; si&eogonek;</trd>
  <trd lng="pt">hesitar</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_l;&c_je;&c_b;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_n;&c_je; &c_r;&c_je;&c_sh;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="sv">tveka</trd>
</drv>
</art>
