<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hezit.xml,v 1.25 2019/02/21 13:10:24 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>hezit</rad>/i
</kap>

<drv mrk="hezit.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Pro dubo &scirc;anceli&gcirc;i, halti; ne scii certe, &ccirc;u oni faros
      a&ubreve; diros ion, a&ubreve; ne:
      <ekz>
        <tld/>i inter du decidoj, partioj;
      </ekz>
      <ekz>
        <tld/>i kun respondo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i pri la finado;
      </ekz>
      <ekz>
        <tld/>i deklamante rolon;
      </ekz>
      <ekz>
        mi <tld/>as preni sur min tiun taskon;
      </ekz>
      <ekz>
        tiu rimarko forte <tld/>igis min;
      </ekz>
      <ekz>
        <ind><tld/>a</ind>j respondoj, pa&scirc;oj.
        <trdgrp lng="de">
          <trd>z&ouml;gernd</trd>,
          <trd>unschl&uuml;ssig</trd>
        </trdgrp>
        <trdgrp lng="es">
          <trd>vacilante</trd>
        </trdgrp>
        <trd lng="fr">h&eacute;sitant</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxance.0igxi">&scirc;anceli&gcirc;i</ref>,
      <ref cel="tim.0i">timi</ref>,
      <ref cel="palp.0i">palpi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_g;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_h;&c_ib;&c_s;&c_t;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol; &c_u; <ind>&c_n;&c_je;&c_r;&c_a;&c_sh;&c_u;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_ib;</ind></trd>
  </trdgrp>
  <trd lng="bg">&c_b;&c_a;&c_v;&c_ja; &c_s;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>b&yacute;t na rozpac&iacute;ch</trd>,
    <trd>kol&iacute;sat</trd>,
    <trd>ot&aacute;let</trd>,
    <trd>rozpakovat se</trd>,
    <trd>v&aacute;hat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>z&ouml;gern</trd>,
    <trd>zaudern</trd>
  </trdgrp>
  <trd lng="el">&delta;&jota;&sigma;&tau;&alfa_ton;&zeta;&omega;</trd>
  <trd lng="en">hesitate</trd>
  <trdgrp lng="es">
    <trd>titubear</trd>,
    <trd>vacilar</trd>
  </trdgrp>
  <trd lng="fr">h&eacute;siter</trd>
  <trdgrp lng="it">
   <trd>esitare</trd>, 
   <trd>titubare</trd> 
  </trdgrp>
  <trd lng="nl">aarzelen</trd>
  <trd lng="pl">waha&cacute; si&eogonek;</trd>
  <trdgrp lng="pt">
   <trd>hesitar</trd>, 
   <trd>vacilar</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_l;&c_je;&c_b;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_n;&c_je; &c_r;&c_je;&c_sh;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ot&aacute;&lcaron;a&tcaron;</trd>,
    <trd>v&aacute;ha&tcaron;</trd>,
    <trd>zdr&aacute;ha&tcaron; sa</trd>
  </trdgrp>
  <trd lng="sv">tveka</trd>
</drv>

<drv mrk="hezit.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Nepovo decidi, alterna prefero al iu a&ubreve; alia eblo, ka&ubreve;zanta
      prokrastanton de ago.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_g;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_h;&c_ib;&c_s;&c_t;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_n;&c_je;&c_r;&c_a;&c_sh;&c_u;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>okolky</trd>,
    <trd>v&aacute;h&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Z&ouml;gern</trd>
  <trdgrp lng="es">
    <trd>duda</trd>,
    <trd>vacilaci&oacute;n</trd>
  </trdgrp>
  <trd lng="fr">h&eacute;sitation</trd>
  <trd lng="it">esitazione</trd>
  <trd lng="pt">hesita&ccedil;&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_l;&c_je;&c_b;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_n;&c_je;&c_r;&c_je;&c_sh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>okolky</trd>,
    <trd>v&aacute;hanie</trd>
  </trdgrp>
</drv>

<drv mrk="hezit.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <dif>
      Ofte ne scianta, kion fari, kion decidi, kion elekti inter pluraj
      ebloj.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_r;&c_a;&c_sh;&c_u;&c_ch;&c_y;</trd>
  <trd lng="cs">v&aacute;hav&yacute;</trd>
  <trdgrp lng="de">
    <trd>z&ouml;gerlich</trd>,
    <trd>zaudernd</trd>,
    <trd>unschl&uuml;ssig</trd>
  </trdgrp>
  <trd lng="es">indeciso</trd>
  <trdgrp lng="fr">
    <trd>h&eacute;sitant</trd>,
    <trd>irr&eacute;solu</trd>
  </trdgrp>
  <trdgrp lng="it">
   <trd>incerto</trd>, 
   <trd>irresoluto</trd> 
  </trdgrp>
  <trd lng="pt">hesitante</trd>
  <trd lng="ru">&c_n;&c_je;&c_r;&c_je;&c_sh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">v&aacute;hav&yacute;</trd>
</drv>

<drv mrk="hezit.sen0a">
  <kap>sen<tld/>a</kap>
  <snc>
    <dif>
      Agpreta, klare scianta kiel agi, paroli, konduti.
    </dif>
    <ref tip="vid" cel="sponta1.0a">spontanea</ref>
  </snc>
  <trdgrp lng="es">
    <trd>decidido</trd>,
    <trd>resuelto</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;cid&eacute;<klr> (r&eacute;solu)</klr></trd>
  <trd lng="it">sicuro<klr tip="amb"> (risoluto)</klr></trd>
  <trd lng="pt">decidido</trd>
  <trd lng="ru">&c_r;&c_je;&c_sh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="hezit.sen0e">
  <kap>sen<tld/>e</kap>
  <snc>
    <dif>
      Tuj agante, kun klarmensa decido:
      <ekz>
        Mi senhezite tuj mendis <ctl>La Novan PIV</ctl>-on, mallonge
        <ctl>PIV2</ctl>
        <fnt><aut>Bertilo Wennergren</aut>: <vrk>Heroa verko &ndash; kun
        mankoj
        (Michel Duc Goninaz, La Nova Plena Ilustrita Vortaro de
        Esperanto)</vrk>, <bib>Monato</bib>, <lok>jaro 2002a, numero
        10a,
        p. 25a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">bez v&aacute;h&aacute;n&iacute;</trd>
  <trdgrp lng="de">
    <trd>ohne zu z&ouml;gern</trd>,
    <trd>ohne Z&ouml;gern</trd>,
    <trd>anstandslos</trd>
  </trdgrp>
  <trd lng="es">decididamente</trd>
  <trd lng="fr">sans <ind>h&eacute;sitation</ind></trd>
  <trdgrp lng="it">
   <trd>decisamente</trd>, 
   <trd>senza <ind>esitazione</ind></trd> 
  </trdgrp>
  <trd lng="pt">sem <ind>hesitar</ind><klr tip="ind"> (adv.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_sh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;</trd>,
    <trd>&c_b;&c_je;&c_z; <ind>&c_k;&c_o;&c_l;&c_je;&c_b;&c_a;&c_n;&c_i;&c_j;</ind></trd>
  </trdgrp>
  <trd lng="sk">bez v&aacute;hania</trd>
</drv>
</art>
