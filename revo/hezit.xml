<drv mrk="hezit.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Pro dubo &scirc;anceli&gcirc;i, halti; ne scii certe, &ccirc;u fari
      a&ubreve; diri ion, a&ubreve; ne:
      <ekz>
        mi estas juna, kaj vi estas maljunuloj, tial mi <tld/>is kaj timis eldiri al vi
        mian opinion
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 32:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne <tld/>is anonci al vi la tutan intencon
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 20:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        la nepreco kaj la simpla prudento subtenis la ka&scirc;itan tenton, &scirc;i &ccirc;esis <tld/>i
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;vi ricevus 100 <klr>[&#8230;]</klr> spesmilojn por jaro&rightquot;,
        <nom>Marta</nom> ne <tld/>is e&ccirc; unu momenton
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>is kun la finado de la demando, sed Marta divenis &scirc;ian penson
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la vicre&gcirc;o <tld/>is fidi al la renegato kaj konfidi al li la kristanojn
        <fnt>
          <bib>DKM</bib>
          <lok>&Ccirc;apitro LXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        se li ne <tld/>is anta&ubreve; tia krimo, pensu, kio ankora&ubreve; povas okazi
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XLVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is pro granda embaraso
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 10</lok>
        </fnt>;
      </ekz>
      <ekz>
        longe li <tld/>is, sed fine, <klr>[&#8230;]</klr> li venki&gcirc;is
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i konscii&gcirc;as pri la nedezirata akompananto, &scirc;i konsterni&gcirc;as, <tld/>as kaj
        rerigardas, poste &scirc;i da&ubreve;rigas la rapidan iron
        <fnt>
          <bib>LGA</bib>
          <lok>II.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> devis ankora&ubreve; unufoje preni lokon en longa viclinio por havigi
        biletojn &ccirc;e unu el la du biletejoj, ili momente <tld/>is por vidi je kiu el
        la du ili havus la plibonan &scirc;ancon
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 7</lok>
        </fnt>;
      </ekz>
      <ekz>
        en Berlino anka&ubreve; oni multe <tld/>is kaj prokrastadis konstruon de la
        metropoliteno
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dub.0i">dubi</ref>,
      <ref cel="embara.0igxi">embarasi&gcirc;i</ref>,
      <ref cel="halt.0i">halti</ref>,
      <ref cel="perple.0igxi">perpleksi&gcirc;i</ref>,
      <ref cel="prokra.0i">prokrasti</ref>,
      <ref cel="sxance.0igxi">&scirc;anceli&gcirc;i</ref>,
      <ref cel="tim.0i">timi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_g;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_h;&c_ib;&c_s;&c_t;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol; &c_u; <ind>&c_n;&c_je;&c_r;&c_a;&c_sh;&c_u;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_ib;</ind></trd>
  </trdgrp>
  <trd lng="bg">&c_b;&c_a;&c_v;&c_ja; &c_s;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>b&yacute;t na rozpac&iacute;ch</trd>,
    <trd>kol&iacute;sat</trd>,
    <trd>ot&aacute;let</trd>,
    <trd>rozpakovat se</trd>,
    <trd>v&aacute;hat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>z&ouml;gern</trd>,
    <trd>zaudern</trd>
  </trdgrp>
  <trd lng="el">&delta;&jota;&sigma;&tau;&alfa_ton;&zeta;&omega;</trd>
  <trd lng="en">hesitate</trd>
  <trdgrp lng="es">
    <trd>titubear</trd>,
    <trd>vacilar</trd>
  </trdgrp>
  <trd lng="fr">h&eacute;siter</trd>
  <trdgrp lng="id">
    <trd><baz>ragu</baz>meragu</trd>,
    <trd><baz>bimbang</baz>membimbang</trd>,
    <trd><baz>sangsi</baz>menyangsi</trd>
  </trdgrp>
  <trdgrp lng="it">
   <trd>esitare</trd>,
   <trd>titubare</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12383;&#12417;&#12425;&#12358;</trd>,
    <trd>&#36490;&#36487;&#12377;&#12427;  <pr>&#12385;&#12421;&#12358;&#12385;&#12423;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">aarzelen</trd>
  <trd lng="pl">waha&cacute; si&eogonek;</trd>
  <trdgrp lng="pt">
   <trd>hesitar</trd>,
   <trd>vacilar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_l;&c_je;&c_b;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_n;&c_je; &c_r;&c_je;&c_sh;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ot&aacute;&lcaron;a&tcaron;</trd>,
    <trd>v&aacute;ha&tcaron;</trd>,
    <trd>zdr&aacute;ha&tcaron; sa</trd>
  </trdgrp>
  <trd lng="sv">tveka</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_a;&c_g;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_b;&c_u;&c_t;&c_i; &c_v; &c_n;&c_je;&c_r;&c_ib;&c_sh;&c_u;&c_ch;&c_o;&c_s;&c_t;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#36485; <pr>zh&uacute;</pr></trd>,
    <trd>&#36831;&#30097; <pr>ch&iacute;y&iacute;</pr></trd>,
    <trd>&#36487; <pr>ch&uacute;</pr></trd>,
    <trd>&#36364;&#36487; <pr>ch&oacute;uch&uacute;</pr></trd>,
    <trd>&#36364;&#36487;&#19981;&#21069; <pr>ch&oacute;uch&uacute;b&ugrave;qi&aacute;n</pr></trd>,
    <trd>&#29359;&#22016;&#21653; <pr>f&agrave;nd&iacute;gu</pr></trd>,
    <trd>&#29369;&#30097; <pr>y&oacute;uy&iacute;</pr></trd>,
    <trd>&#30031;&#36991; <pr>w&egrave;ib&igrave;</pr></trd>,
    <trd>&#36228; <pr>j&umacron;</pr></trd>,
    <trd>&#30861;&#21475; <pr>&agrave;ik&#466;u</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hezit.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      <tld lit="H"/>anta; esprimanta <tld/>adon:
      <ekz>
        kun <tld/>a demando en la okuloj li ree rigardis al la patrino, sed &scirc;i kurbi&gcirc;is
        super sia laboro kaj e&ccirc; ne rimarkis lin
        <fnt>
          <bib>HsT</bib>
          <lok>Heredanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Alda</nom> atente rigardis Alekson, kaj &scirc;ia viza&gcirc;esprimo nuanci&gcirc;is de <tld/>a parolemo
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u estis <tld/>a pa&ubreve;zo a&ubreve; nur la normala tempospaco inter demando kaj respondo
        <fnt>
          <bib>CKv</bib>
          <lok>23</lok>
        </fnt>?
      </ekz>
      <ekz>
        virinoj pu&scirc;adis sian &ccirc;areton de sekcio al sekcio, haltante tie kaj &ccirc;i tie,
        farante jen kaj jen <tld/>an dancon, kvaza&ubreve; abeloj flugantaj de floro al
        floro por elekti la plej ta&ubreve;gan nektaron
        <fnt>
          <bib>ChV</bib>
          <lok>13</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gxen.sin0a.hontema">sin&gcirc;ena<sncref/></ref>,
      <ref cel="hont.0ema">hontema</ref>,
      <ref cel="timid.0a">timida</ref>
    </refgrp>
    <trd lng="be">&c_n;&c_je;&c_r;&c_a;&c_sh;&c_u;&c_ch;&c_y;</trd>
    <trdgrp lng="de">
      <trd>z&ouml;gernd</trd>,
      <trd>z&ouml;gerlich</trd>,
      <trd>unschl&uuml;ssig</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>vacilante</trd>
    </trdgrp>
    <trd lng="fr">h&eacute;sitant</trd>
  </snc>
  <trd lng="en">hesitant</trd>
  <trdgrp lng="id">
    <trd>ragu</trd>,
    <trd>ragu-ragu</trd>,
    <trd>bimbang</trd>,
    <trd>sangsi</trd>,
    <trd>syak</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#29369;&#35947;&#19981;&#20915; <pr>y&oacute;uy&ugrave;b&ugrave;ju&eacute;</pr></trd>,
    <trd>&#29369;&#35947; <pr>y&oacute;uy&ugrave;</pr></trd>,
    <trd>&#36383; <pr>ch&iacute;</pr></trd>,
    <trd>&#29369;&#35947;&#22320; <pr>y&oacute;uy&ugrave;de</pr></trd>,
    <trd>&#36831;&#30097;&#22320; <pr>ch&iacute;y&iacute;de</pr></trd>,
    <trd>&#36399; <pr>zh&iacute;</pr></trd>,
    <trd>&#20330; <pr>hu&aacute;i</pr></trd>,
    <trd>&#20739; <pr>ch&aacute;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hezit.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Nepovo decidi, alterna prefero al iu a&ubreve; alia eblo, ka&ubreve;zanta
      <ref cel="prokra.0o">prokrasto</ref>n de ago:
      <ekz>
        la mastrino, penetrema kaj vigla, tuj kaptis la momenton de embarasi&gcirc;o kaj
        <tld/>o de la kolegino
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        diru al mi sincere kaj sen <tld/>o <klr>[&#8230;]</klr>
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        post <tld/>o &scirc;i da&ubreve;rigis: &ndash; &ccirc;u vi ne revis pri kisoj, sentis deziron
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun la sama komuna iniciato en la sama momento, sen <tld/>o kaj sen &gcirc;eno kaj
        sentante nur ian mirindan fratecon simple ili interkisi&gcirc;is
        <fnt>
          <bib>LGA</bib>
          <lok>III.</lok>
        </fnt>;
      </ekz>
      <ekz>
        stranga sento de <tld/>o kaj honteto ekkaptis lin
        <fnt>
          <bib>Tor</bib>
          <lok>VI.</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="moment.0o">momento</ref>,
      <ref cel="pauxz.0o">pa&ubreve;zo</ref>,
      <ref cel="rezerv.0o.ago">rezervo<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_g;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_h;&c_ib;&c_s;&c_t;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_n;&c_je;&c_r;&c_a;&c_sh;&c_u;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>okolky</trd>,
    <trd>v&aacute;h&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Z&ouml;gern</trd>
  <trd lng="en">hesitation</trd>
  <trdgrp lng="es">
    <trd>duda</trd>,
    <trd>vacilaci&oacute;n</trd>
  </trdgrp>
  <trd lng="fr">h&eacute;sitation</trd>
  <trdgrp lng="id">
    <trd><baz>ragu</baz>keraguan</trd>,
    <trd><baz>bimbang</baz>kebimbangan</trd>,
    <trd><baz>sangsi</baz>kesangsian</trd>,
    <trd>syak</trd>
  </trdgrp>
  <trd lng="it">esitazione</trd>
  <trdgrp lng="ja">
    <trd>&#12383;&#12417;&#12425;&#12356;</trd>,
    <trd>&#36490;&#36487;  <pr>&#12385;&#12421;&#12358;&#12385;&#12423;</pr></trd>
  </trdgrp>
  <trd lng="pt">hesita&ccedil;&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_l;&c_je;&c_b;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_n;&c_je;&c_r;&c_je;&c_sh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>okolky</trd>,
    <trd>v&aacute;hanie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_a;&c_g;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_n;&c_je;&c_r;&c_ib;&c_sh;&c_u;&c_ch;&c_ib;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="zh">&#29369;&#35947; <pr>y&oacute;uy&ugrave;</pr></trd>
</drv>

<drv mrk="hezit.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <dif>
      Inklina prokrasti decidon, faron pro necerteco, malfacila elekto ks:
      <ekz>
        &scirc;teliris junulo kaj junulino al la silenta bosko kun la pla&ubreve;danta fonteto por
        flustri unu al la alia la <tld/>emajn vortojn, kiujn fortimigis la akra lumo
        de la tago
        <fnt>
          <bib>Tor</bib>
          <lok>II.</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis pala kaj <tld/>ema ankora&ubreve; kaj la rerideto de la knabino estis tre
        malgaja
        <fnt>
          <bib>LGA</bib>
          <lok>V.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Ilu-ulba</nom> divenis iom el la pensoj de la <tld/>ema re&gcirc;o
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_r;&c_a;&c_sh;&c_u;&c_ch;&c_y;</trd>
  <trd lng="cs">v&aacute;hav&yacute;</trd>
  <trdgrp lng="de">
    <trd>z&ouml;gerlich</trd>,
    <trd>zaudernd</trd>,
    <trd>unschl&uuml;ssig</trd>
  </trdgrp>
  <trd lng="en">hesitative</trd>
  <trd lng="es">indeciso</trd>
  <trdgrp lng="fr">
    <trd>h&eacute;sitant</trd>,
    <trd>irr&eacute;solu</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>mudah ragu</trd>,
    <trd>mudah bimbang</trd>
  </trdgrp>
  <trdgrp lng="it">
   <trd>incerto</trd>,
   <trd>irresoluto</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12383;&#12417;&#12425;&#12356;&#12364;&#12385;&#12394;</trd>,
    <trd>&#20778;&#26580;&#19981;&#26029;&#12394;  <pr>&#12422;&#12358;&#12376;&#12421;&#12358;&#12405;&#12384;&#12435;&#12394;</pr></trd>
  </trdgrp>
  <trd lng="pt">hesitante</trd>
  <trd lng="ru">&c_n;&c_je;&c_r;&c_je;&c_sh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">v&aacute;hav&yacute;</trd>
  <trd lng="uk">&c_n;&c_je;&c_r;&c_ib;&c_sh;&c_u;&c_ch;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#20330; <pr>hu&aacute;i</pr></trd>,
    <trd>&#24492; <pr>p&aacute;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hezit.sen0a">
  <kap>sen<tld/>a</kap>
  <snc>
    <dif>
      Agpreta, klare scianta kiel agi, paroli, konduti:
      <ekz>
        ja mi ordonis al vi, <klr>[do]</klr> estu sen<tld/>aj kaj kura&gcirc;aj
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 13:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        flegma kaj sen<tld/>a torturisto
        <fnt>
          <bib>MkM</bib>
          <lok>2</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;efju&gcirc;isto de la Supera Kortumo kun sen<tld/>a mieno akceptis la prezidantan
        &jcirc;uron
        <fnt>
          <bib>Monato</bib>
          <aut>Alexander Gofen</aut>
          <vrk><url ref="&Monato;/2009/009811.php">Obamena&jcirc;o, a&ubreve; haladzo de socialisma
          aventurismo</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rekt.0a.senpera">rekta<sncref/></ref>,
      <ref cel="per.sen0a">senpera</ref>,
      <ref cel="sponta1.0a">spontanea</ref>,
      <ref cel="tuj.0a">tuja</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_sh;&c_u;&c_ch;&c_y;</trd>
  <trdgrp lng="de">
    <trd>unmittelbar</trd>,
    <trd>anstandslos</trd>,
    <trd>ohne zu <ind>z&ouml;gern</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>prompt <klr>(decisive)</klr></trd>,
    <trd>decisive <klr>(prompt)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>decidido</trd>,
    <trd>resuelto</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;cid&eacute;<klr> (r&eacute;solu)</klr></trd>
  <trdgrp lng="id">
    <trd>yakin</trd>,
    <trd>tanpa ragu</trd>
  </trdgrp>
  <trd lng="it">sicuro<klr tip="amb"> (risoluto)</klr></trd>
  <trd lng="pt">decidido</trd>
  <trd lng="ru">&c_r;&c_je;&c_sh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#26524;&#26029; <pr>gu&#466;du&agrave;n</pr></trd>,
    <trd>&#27627;&#19981;&#36364;&#36487;&#22320; <pr>h&aacute;ob&ugrave;ch&oacute;uch&uacute;de</pr></trd>,
    <trd>&#19981;&#21152;&#24605;&#32034; <pr>b&ugrave;ji&amacron;s&imacron;su&#466;</pr></trd>,
    <trd>&#31435;&#21363; <pr>l&igrave;j&iacute;</pr></trd>,
    <trd>&#22362;&#20915; <pr>ji&amacron;nju&eacute;</pr></trd>
  </trdgrp>
