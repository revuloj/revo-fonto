<art mrk="$Id: poetik.xml,v 1.17 2022/04/16 22:29:56 revo Exp $">
<kap>
  <rad>poetik</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="poetik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POE</uzo>
  <snc mrk="poetik.0o.POE">
    <dif>
      Teorio pri <ref tip="vid" cel="poezi.0o.arto">poezio</ref>:
      <ekz>
        la tri plej gravaj projektoj efektivigotaj de tiu fama Kompanio
        estas unu Gramatiko, unu Retoriko kaj unu <tld lit="P"/>o
        <fnt><bib>PGl</bib>, <lok>Skizo de Esperanta Metriko, p.
        11a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_e;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="cs">poetika</trd>
  <trd lng="de">Poetik</trd>
  <trd lng="fr">po&eacute;tique<klr tip="amb"> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>verstan</trd>,
    <trd>k&ouml;lt&eacute;szettan</trd>,
    <trd>po&eacute;tika</trd>
  </trdgrp>
  <trd lng="nl">po&euml;tica</trd>
  <trd lng="pl">poetyka</trd>
  <trd lng="pt">po&eacute;tica</trd>
  <trd lng="ro">arta  <ind>poetic&abreve;</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_e;&c_t;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_je;&c_o;&c_r;&c_i;&c_ja; &c_s;&c_t;&c_i;&c_h;&c_o;&c_s;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">poetika</trd>
  <trd lng="uk">&#x43F;&#x43E;&c_je;&c_t;&c_i;&#x43A;&c_a;</trd>
</drv>
</art>
