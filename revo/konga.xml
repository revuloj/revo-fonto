<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>konga</rad>/o <fnt><bib>RokPop</bib></fnt>
</kap>
<drv mrk="konga.0o">
  <kap><tld/>o</kap>
  <snc mrk="konga.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Kubdevena
      <ref tip="lst" cel="instru2.frap0o"
      lst="voko:frapinstrumentoj">frapinstrumento</ref>,
      alta tamburo kun konveksa flanko, supra leda membrano,
      suba malferma&jcirc;o, plejofte manfrapata:
      <ekz>
        <tld/>o estas proks<klr>imume</klr> 70-90 cm alta mantamburo
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Kongao">
          kongao</url>, 2011-12-06</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">conga</trd>
  <trd lng="de">Conga<klr tip="amb"> (Trommel)</klr></trd>
  <trd lng="fr">conga<klr> (instr. de musique)</klr></trd>
</drv>
</art>

