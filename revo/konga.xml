<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: konga.xml,v 1.9 2024/05/23 19:24:30 revo Exp $">
<kap>
  <rad>konga</rad>/o <fnt><bib>RokPop</bib></fnt>
</kap>
<drv mrk="konga.0o">
  <kap><tld/>o</kap>
  <snc mrk="konga.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Kubdevena
      <ref tip="lst" cel="instru2.frap0o"
      lst="voko:frapinstrumentoj">frapinstrumento</ref>,
      alta tamburo kun konveksa flanko, supra leda membrano,
      suba malferma&jcirc;o, plejofte manfrapata:
      <ekz>
        <tld/>o estas proks<klr>imume</klr> 70-90 cm alta mantamburo
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Kongao">
          kongao</url>, 2011-12-06</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_o;&c_n;&c_g;&c_a; <klr tip="amb">(&c_m;&c_u;&c_z;. &c_ib;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_e;&c_n;&c_t;)</klr></trd>,
    <trd>&c_t;&c_u;&c_m;&c_b;&c_a;&c_d;&c_o;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">conga</trd>
  <trdgrp lng="cs">
    <trd>conga</trd>,
    <trd>konga</trd>
  </trdgrp>
  <trd lng="de">Conga<klr tip="amb"> (Trommel)</klr></trd>
  <trd lng="fr">conga<klr> (instr. de musique)</klr></trd>
  <trd lng="pl">kongi</trd>
  <trd lng="sk">konga</trd>
</drv>
</art>

