<drv mrk="pardon.0u">
  <kap><tld/>u,
    <var>
      <kap>pardonon</kap>
    </var>
  </kap>
  <snc mrk="pardon.0u.KOMUNE">
    <dif>
      Peto toleri senkolere farotan malagrabla&jcirc;on, &gcirc;enon,
      maloportuna&jcirc;on, kulpon:
      <ekz>
        <tld/>u min, ke mi nur aludas <klr>[tion]</klr>
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Tria Kongreso Esperantista en Cambridge en la 12a de a&ubreve;gusto 1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u, vi ne lasis min elparoli &gcirc;is la fino
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi petas <tld/>on, Doktoro, <klr>[&#8230;]</klr> &ccirc;u estas permesite malhelpi
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>?
      </ekz>
      <ekz>
        <tld/>on! <tld/>on! mi vin tre ofendis
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u, se mi vin nevole ofendis
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_b;&c_a;&c_ch;&c_a;&c_j;&c_c;&c_je;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_p;&c_r;&c_a;&c_sh;&c_a;&c_ju;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>entschuldige!</trd>,
    <trd>Verzeiihung!</trd>
  </trdgrp>
  <trd lng="en">sorry</trd>
  <trd lng="fr">excusez-moi</trd>
  <trdgrp lng="id">
    <trd><ind>maaf</ind>!</trd>,
    <trd><ind>permisi</ind>!</trd>
  </trdgrp>
  <trd lng="pl">przepraszam</trd>
  <trd lng="ro">scuz&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_i;&c_t;&c_je;</trd>,
    <trd>&c_i;&c_z;&c_v;&c_i;&c_n;&c_i;&c_t;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_sh;&c_u; &c_p;&c_r;&c_o;&c_shch;&c_je;&c_n;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="zh">&#23545;&#19981;&#36215;<klr> [du&igrave;b&ugrave;q&#464;]</klr>, &#21171;&#39550;<klr>
  [l&aacute;oji&agrave;]</klr></trd>
</drv>

<drv mrk="pardon.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="pardon.0ema.KOMUNE">
    <dif>
      Inklina al <tld/>o:
      <ekz>
        animo pura, <tld/>ema
        <fnt>
          <bib>VivZam</bib>
          <lok>Vivo de Zamenhof</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estas tre <tld/>ema, li ne portas longe la koleron kaj li tute ne estas
        ven&gcirc;ema
        <fnt>
          <bib>F</bib>
          <lok>&FE; 41</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Allah</nom> estas senkulpigema, <tld/>ema
        <fnt>
          <aut>trad. I. Chiussi</aut>,
          <vrk>La Nobla Korano</vrk>,
          <lok>1970</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="anim.grand0a">grandanima</ref>,
      <ref cel="indulg.0ema">indulgema</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_r;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trd lng="ca">perdonador</trd>
