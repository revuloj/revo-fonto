  <trdgrp lng="hu">
      <trd>megbocs&aacute;t&oacute;</trd>,
      <trd>b&eacute;k&uuml;l&eacute;keny</trd>
    </trdgrp>
  <trdgrp lng="id">
    <trd><baz>ampun</baz>pengampun</trd>,
    <trd><baz>maaf</baz>pemaaf</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#23515;&#22823;&#12394;  <pr>&#12363;&#12435;&#12384;&#12356;&#12394;</pr></trd>,
    <trd>&#23515;&#23481;&#12394;  <pr>&#12363;&#12435;&#12424;&#12358;&#12394;</pr></trd>
  </trdgrp>
  <trd lng="nl">vergevingsgezind</trd>
  <trdgrp lng="pl">
    <trd>mi&lstroke;osierny</trd>,
    <trd>mi&lstroke;o&sacute;ciwy</trd>
  </trdgrp>
  <trd lng="ro">milostiv</trd>
  <trd lng="ru">&c_s;&c_n;&c_i;&c_s;&c_h;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
