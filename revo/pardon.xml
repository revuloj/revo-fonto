  <trdgrp lng="id">
    <trd><baz>ampun</baz>ampunan</trd>,
    <trd>amnesti</trd>,
    <trd>maaf</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12422;&#12427;&#12375;</trd>,
    <trd>&#23481;&#36198;  <pr>&#12424;&#12358;&#12375;&#12419;</pr></trd>,
    <trd>&#21208;&#24321;  <pr>&#12363;&#12435;&#12409;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="lat">uenia, ae</trd>
  <trd lng="nl">vergeving</trd>
  <trdgrp lng="pl">
