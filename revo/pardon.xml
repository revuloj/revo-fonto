  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_b;&c_a;&c_ch;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_r;&#x43E;&c_b;&c_a;&c_ch;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_r;&#x43E;&c_shch;&c_a;&c_t;&c_i;</trd>,
