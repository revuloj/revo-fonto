    <trd>&#39286;&#24661; <pr>r&aacute;osh&ugrave;</pr></trd>,
    <trd>&#21253;&#23481; <pr>b&amacron;or&oacute;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pardon.0o">
  <kap><tld/>o</kap>
  <snc mrk="pardon.0o.KOMUNE">
    <dif>
      Ago <tld/>i:
      <ekz>
        la instruo de Kristo estas <tld/>o, amo kaj favorkoreco
        <fnt>
          <bib>Fab3</bib>
          <lok>En la lasta tago</lok>
        </fnt>!
      </ekz>
      <ekz>
        mi estas preta sur la genuoj peti <tld/>on de vi
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        fajron estingas akvo, pekon <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
      <trd>&c_d;&c_a;&c_r;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_b;&c_a;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
  <trd lng="br">pardon</trd>
  <trdgrp lng="ca">
      <trd>perd&oacute;</trd>,
      <trd>perdonan&ccedil;a</trd>
    </trdgrp>
  <trdgrp lng="cs">
  <trd>odpu&scaron;t&ecaron;n&iacute;</trd>,
  <trd>pardon</trd>,
  <trd>prominut&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
