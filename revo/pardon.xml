      <trd>verzeihen</trd>
    </trdgrp>
  <trd lng="en">pardon</trd>
  <trd lng="es">perdonar</trd>
  <trdgrp lng="fr">
      <trd>excuser</trd>,
      <trd>pardonner</trd>
    </trdgrp>
  <trd lng="hu">megbocs&aacute;t</trd>
  <trdgrp lng="id">
    <trd><baz>ampun</baz>mengampuni</trd>,
    <trd><baz>maaf</baz>memaafkan</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12422;&#x308B;&#12377;</trd>,
    <trd>&#x5BB9;&#x8D66;&#12377;&#x308B;  <pr>&#12424;&#12358;&#12375;&#12419;&#12377;&#x308B;</pr></trd>,
    <trd>&#x52D8;&#x5F01;&#12377;&#x308B;  <pr>&#x304B;&#12435;&#12409;&#12435;&#12377;&#x308B;</pr></trd>,
    <trd>&#22823;&#x76EE;&#x306B;&#x898B;&#x308B;  <pr>&#x304A;&#x304A;&#12417;&#x306B;&#x307F;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="lat">
