<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc><rad>pardon</rad>/i
    </kap>

    <drv mrk="pardon.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>x</vspec></gra>
      <snc mrk="pardon.0i.senkulpigi">
        <dif>
          Forvi&scirc;i el sia menso ofendon, malican senton, koleron
          kontra&ubreve; persono kulpa kontra&ubreve; la <tld/>anto:
          <ekz>
            <tld/>u, ke mi &gcirc;is nun ne respondis vian
            leteron<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>u al mi, ke mi restis tiel longe<fnt>Z</fnt>;
          </ekz><ekz>
            &ccirc;ar se vi <tld/>as al homoj iliajn kulpojn, via Patro
            &ccirc;iela anka&ubreve; <tld/>os al vi
	    <fnt><bib>NT</bib>, <lok>&Mat; 6:14</lok></fnt>.
          </ekz>
        </dif>
        <trdgrp lng="ru">
          <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>,
          <trd>&c_p;&c_r;&c_o;&c_shch;&c_a;&c_t;&c_mol;</trd>,
          <trd>&c_i;&c_z;&c_v;&c_i;&c_n;&c_i;&c_t;&c_mol;</trd>,
          <trd>&c_i;&c_z;&c_v;&c_i;&c_n;&c_ja;&c_t;&c_mol;</trd>
        </trdgrp>
      </snc>
      <snc mrk="pardon.0i.indulgi">
        <dif>
          Rezigni punon a&ubreve; ven&gcirc;on pro suferita ofendo a&ubreve; maljusta&jcirc;o:
          <ekz>
            <tld/>i ion al iu;
          </ekz><ekz>
            <uzo tip="fak">JUR</uzo>
            post la demisio de prezidento Nixon, la sekva prezidento
            Gerald Ford <tld/>is al li &ccirc;iujn federajn krimojn kiujn
            li povis fari en sia ofico;
          </ekz><ekz>
            la popolamaso preferis <tld/>i Barabon ol Jesuon;
          </ekz><ekz>
            mi ne <tld/>as, ke vi malobeis;
          </ekz><ekz>
            ne<tld/>ebla krimo.
          </ekz>
        </dif>
        <ref tip="sin" cel="indulg.0i">indulgi</ref>
        <ref tip="vid" cel="amnest.0o">amnestio</ref>
        <trdgrp lng="ru">
          <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>,
          <trd>&c_p;&c_o;&c_m;&c_i;&c_l;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
        </trdgrp>
      </snc>
      <snc mrk="pardon.0i.sxuldon">
        <dif>
          Liberigi iun el devigo, nuligi &scirc;uldon:
          <ekz>
            <tld/>u al ni &scirc;uldojn niajn, kiel ni anka&ubreve; <tld/>as al
            niaj &scirc;uldantoj
            <fnt>
              <aut>Tr. &Z;</aut>: <vrk>Patro nia</vrk>.
              <lok>La Unua Libro (&c_D;-&c_r;&c_malmol; &c_E;&c_s;&c_p;&c_je;&c_r;&c_a;&c_n;&c_t;&c_o;: &c_M;&c_je;&c_zh;&c_d;&c_u;&c_n;&c_a;&c_r;&c_o;&c_d;&c_n;&c_y;&c_j;
                &c_ja;&c_z;&c_y;&c_k;&c_malmol;. &c_P;&c_r;&c_je;&c_d;&c_i;&c_s;&c_l;&c_o;&c_v;&c_ib;&c_je; &c_i; &c_p;&c_o;&c_l;&c_n;&c_y;&c_j; &c_u;&c_ch;&c_je;&c_b;&c_n;&c_i;&c_k;&c_malmol;. (por
                Rusoj). Varsovio, 1887. P&gcirc;. 19a.</lok>
            </fnt>
          </ekz>
        </dif>
        <trdgrp lng="ru">
          <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>,
          <trd>&c_p;&c_r;&c_o;&c_shch;&c_a;&c_t;&c_mol;</trd>
        </trdgrp>
      </snc>
      <trd lng="ca">perdonar</trd>
      <trdgrp lng="be">
        <trd>&c_d;&c_a;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_p;&c_r;&c_a;&c_b;&c_a;&c_ch;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_v;&c_y;&c_b;&c_a;&c_ch;&c_a;&c_c;&c_mol;</trd>
      </trdgrp>
      <trd lng="br">pardoni&ntilde;</trd> 
      <trdgrp lng="cs">
        <trd>prominout</trd>,
        <trd>odpustit</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>vergeben</trd>,
        <trd>verzeihen</trd>
      </trdgrp>
      <trd lng="en">pardon</trd>
      <trd lng="es">perdonar</trd>
      <trdgrp lng="fr">
        <trd>excuser</trd>,
        <trd>pardonner</trd>
      </trdgrp>
      <trd lng="hu">megbocs&aacute;t</trd>  
      <trdgrp lng="lat">
        <trd>excuso, are</trd>;
        <trd>ignosco, ere</trd>
      </trdgrp>
      <trd lng="nl">vergeven</trd>
      <trd lng="pt">perdoar</trd>
    </drv>

    <drv mrk="pardon.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Ago <tld/>i:
          <ekz>
            peti <tld/>on de iu.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_d;&c_a;&c_r;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
        <trd>&c_p;&c_r;&c_a;&c_b;&c_a;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
      </trdgrp>      
      <trd lng="br">pardon</trd>
      <trdgrp lng="ca">
        <trd>perd&oacute;</trd>,
        <trd>perdonan&ccedil;a</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Vergebung</trd>,
        <trd>Verzeihung</trd>
      </trdgrp>
      <trd lng="es">perd&oacute;n</trd>
      <trd lng="fr">pardon</trd>
      <trdgrp lng="hu">
        <trd>megbocs&aacute;t&aacute;s</trd>,
        <trd>bocs&aacute;nat</trd>,
        <trd>pardon</trd>
      </trdgrp>
      <trd lng="lat">uenia, ae</trd>
      <trd lng="nl">vergeving</trd>
      <trdgrp lng="ru">
        <trd>&c_p;&c_r;&c_o;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>,
        <trd>&c_i;&c_z;&c_v;&c_i;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
      </trdgrp>
    </drv>

    <drv mrk="pardon.0u">
      <kap><tld/>u,
        <var>
          <kap>pardonon</kap>
        </var>
      </kap>
      <snc>
        <dif>
          Peto toleri senkolere farotan malagrabla&jcirc;on, &gcirc;enon,
          maloportuna&jcirc;on:
          <ekz>
            <tld/>u min, ke mi nur aludas tion<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>u, &ccirc;u vi bonvolos doni al mi la salujon?
          </ekz>
        </dif>
      </snc>
      <trd lng="en">sorry</trd>
      <trdgrp lng="ru">
        <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_i;&c_t;&c_je;</trd>,
        <trd>&c_i;&c_z;&c_v;&c_i;&c_n;&c_i;&c_t;&c_je;</trd>,
        <trd>&c_p;&c_r;&c_o;&c_sh;&c_u; &c_p;&c_r;&c_o;&c_shch;&c_je;&c_n;&c_i;&c_ja;</trd>
      </trdgrp>
    </drv>

    <drv mrk="pardon.0ema">
      <kap><tld/>ema</kap>
      <snc>
        <dif>
          Inklina al <tld/>o:
          <ekz>
            <tld/>emeco superas justecon
            <fnt><bib>PrV</bib></fnt>.</ekz>
        </dif>
      </snc>
      <trd lng="be">&c_d;&c_a;&c_r;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
      <trd lng="ca">perdonador</trd>
      <trdgrp lng="hu">
        <trd>megbocs&aacute;t&oacute;</trd>,
        <trd>b&eacute;k&uuml;l&eacute;keny</trd>
      </trdgrp>
      <trd lng="nl">vergevingsgezind</trd>
      <trd lng="ru">&c_s;&c_n;&c_i;&c_s;&c_h;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </drv>
  </art>
