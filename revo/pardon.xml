    <trd>&c_n;&c_je;&c_p;&c_r;&c_o;&c_s;&c_t;&c_i;&c_m;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="pardon.0peti">
  <kap><tld/>peti <fnt><bib>PIV1</bib></fnt></kap>
  <snc>
    <dif>
      Pro &gcirc;entileco peti ies <tld/>on:
      <ekz>
        &ccirc;u li povas <tld/>peti pri siaj propraj pekoj
        <fnt>
          <aut>Tr. G. Berveling</aut>:
          <vrk>La sa&gcirc;eco de Jesuo Sira&hcirc;</vrk>. <lok>28:4</lok>
        </fnt>?
      </ekz><ekz>
        mi devas <tld/>peti pro erareto en mia lasta letero
        <fnt><bib>Monato</bib>, <aut>Frieder Weigold</aut></fnt>;
      </ekz><ekz>
        Tamen, kiam mi relegas la modernajn librojn, enkore mi
        <tld/>petas la Budape&scirc;tan korifeon
        <fnt><aut>Karolo Pi&ccirc;</aut>: <vrk>Kolomano Kalocsay</vrk>
          <lok>Nica literatura revuo,  6/6 (n-ro 36)</lok></fnt>;
      </ekz><ekz>
         mi devas <tld/>peti de la regnanoj de la civilizita angla
         &scirc;tato, ke mi ilin tedas per la priskribo de tiaj idiota&jcirc;oj
         <fnt>
          <aut>S&aacute;ndor Szathm&aacute;ri</aut>:
          <vrk>Voja&gcirc;o al Kazohinio</vrk>, <lok>1938. &ccirc;ap. 10-a</lok>
        </fnt>;
      </ekz><ekz>
        tial mi <tld/>petas al &ccirc;iuj menciitaj en la letero
        <fnt><aut>Jouko Lindstedt</aut>:
          <vrk>Nefermita plendoletero</vrk>, <bib>LOdE</bib>,
          <lok>2001. &#8470;2</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="ekskuz.0i">ekskuzi</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_b;&c_a;&c_ch;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_s;&c_ib;&c_c;&c_mol; <ind>&c_p;&c_r;&c_a;&c_b;&c_a;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_ja;</ind></trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_p;&c_r;&c_a;&c_sh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>um <ind>Verzeihung</ind> bitten</trd>,
    <trd>um <ind>Vergebung</ind> bitten</trd>,
    <trd>sich <ind>entschuldigen</ind></trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>minta</ind> maaf</trd>,
    <trd><ind>minta</ind> ampun</trd>,
    <trd><baz>minta</baz>meminta maaf</trd>,
    <trd><baz>minta</baz>meminta ampun</trd>,
    <trd><ind>mohon</ind> maaf</trd>,
    <trd><ind>mohon</ind> ampun</trd>,
    <trd><baz>mohon</baz>memohon maaf</trd>,
    <trd><baz>mohon</baz>memohon ampun</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12422;&#12427;&#12375;&#12434;&#35531;&#12358;  <pr>&#12422;&#12427;&#12375;&#12434;&#12371;&#12358;</pr></trd>,
    <trd>&#12431;&#12403;&#12427;</trd>,
    <trd>&#35613;&#32618;&#12377;&#12427;  <pr>&#12375;&#12419;&#12374;&#12356;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przeprasza&cacute;</trd>,
    <trd>prosi&cacute; o wybaczenie</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>scuza&tcomma;i</trd>,
    <trd>cere iertare</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_v;&c_i;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_i;&c_t;&c_mol; &c_p;&c_r;&c_o;&c_shch;&c_je;&c_n;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="uk">&c_p;&c_r;&c_o;&c_s;&c_i;&c_t;&c_i;
  &c_p;&c_r;&c_o;&c_b;&c_a;&c_ch;&c_je;&c_n;&c_n;&c_ja;/&c_v;&c_i;&c_b;&c_a;&c_ch;&c_je;&c_n;&c_n;&c_ja;</trd>
</drv>

