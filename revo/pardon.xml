      <trd>&c_i;&c_z;&c_v;&c_i;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  <trdgrp lng="sk">
  <trd>milos&tcaron;</trd>,
  <trd>odpustenie</trd>,
  <trd>prep&aacute;&ccaron;enie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_b;&c_a;&c_ch;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_b;&c_a;&c_ch;&c_je;&c_n;&c_n;&c_ja;</trd>,
