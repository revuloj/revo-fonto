<drv mrk="variac.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="vari.0ajxo">Varia&jcirc;o</ref>:
      <ekz>
        <uzo tip="fak">MUZ</uzo>
