<art mrk="$Id: drent.xml,v 1.11 2023/11/04 18:50:51 revo Exp $">
<kap>
<rad>Drent</rad>/o
</kap>
<drv mrk="drent.D0o">
  <kap><tld/>o</kap>
  <fnt><bib>JuanReg</bib></fnt>
  <snc>
  <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="super" cel="provin.0o">Provinco</ref> en norda  
      <ref tip="malprt" cel="nederl.N0o">Nederlando</ref>, kun &ccirc;efurbo
      <ref tip="prt" cel="asen.A0o">Aseno</ref>:
      <ekz>
        <tld/>o estas Nederlanda provinco, kiu situas en la nordorienta parto de la lando
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="&Viki;/Drento">Drento</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la rivero Reitdiep <klr>(...)</klr> estas grava por la drenado de la
        troa akvo en grandaj parto de la provinco Groningeno
        kaj la norda parto de <tld/>o
        <fnt>
          <aut>Luit Staghouwer</aut>
          <vrk><url ref="https://www.pingveno.nl/klubo/LVS%20apr%2011.pdf#page=12">
            Pri Reitdiep kaj la kluzo de Schapehals</url></vrk>
          <lok>La Verda Stelo, aprilo 2011, pa&gcirc;o 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        en Gelrio, Nord-Brabanto, <tld/>o ktp., mi ne malofte vidis &gcirc;in
        <fnt>
          <aut>Wouter PILGER</aut>:
          <vrk>Nederlandaj venenaj kreska&jcirc;oj</vrk>, 
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_D;&c_r;&c_e;&c_n;&c_t;&c_e;</trd>
  <trd lng="en">Drenthe</trd>
  <trd lng="de">Drenthe</trd>
  <trd lng="fr">Drenthe</trd>
  <trd lng="fy">Drinte</trd>
  <trd lng="hu">Drente</trd>
  <trd lng="ja">&#x30C9;&#x30EC;&#x30F3;&#x30C6;&#x5DDE; <pr>&#x3069;&#x308C;&#x3093;&#x3066;&#x3057;&#x3085;&#x3046;</pr></trd>
  <trd lng="ko">&#xB4DC;&#xB80C;&#xD130;&#xC8FC;</trd>
  <trd lng="nl">Drenthe</trd>
  <trd lng="pt">Drente</trd>
  <trd lng="ru">&c_D;&c_r;&c_je;&c_n;&c_t;&c_je;</trd>
  <trd lng="zh">&#x5FB7;&#x4F26;&#x7279;&#x7701; <pr>d&#xE9;l&#xFA;nt&#xE9;x&#x1D0;ng</pr></trd>
</drv>
</art>

