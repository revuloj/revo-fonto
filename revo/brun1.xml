<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: brun1.xml,v 1.10 2024/12/21 10:25:58 revo Exp $">
<kap>
  <rad>Brun</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="brun1.0o">
  <kap><tld/>o</kap>
  <snc mrk="brun1.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        li konas <tld/>on, nian &ccirc;efon
        <fnt>
          <bib>ChR</bib>
          <lok>Kiu pafis al Pipulo?</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o studis &ccirc;e la katedralaj lernejoj en Kolonjo kaj
        <nom>Reims</nom>
        <fnt>
        <bib>Viki</bib>,
        <lok>Bruno de Kolonjo, 2005-07-13</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">Bruno</trd>
    <trd lng="fr">Bruno</trd>
    <trd lng="it">Bruno</trd>
    <trd lng="pl">Bruno</trd>
  </snc>
  <ref tip="hom" cel="brn.0o">Bruno (urbo Brno)</ref>
  <trd lng="be">&c_B;&c_r;&c_u;&c_n;&c_a; <klr>(&c_ib;&c_m;&c_ja;)</klr></trd>
  <trd lng="cs">Giordano Bruno</trd>
  <trd lng="de">Bruno</trd>
  <trd lng="sk">Giordano Bruno</trd>
  <trd lng="uk">&c_B;&c_r;&c_u;&c_n;&c_o;&nbsp;(&c_ib;&c_m;&#8217;&c_ja;)</trd>
</drv>
</art>

