    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        <tld/>o studis &ccirc;e la katedralaj lernejoj en Kolonjo kaj
        Reims
        <fnt>
        <bib>Viki</bib>,
        <lok>Bruno de Kolonjo, 2005-07-13</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">Bruno</trd>
    <trd lng="fr">Bruno</trd>
    <trd lng="it">Bruno</trd>
    <trd lng="pl">Bruno</trd>
  </snc>
  <trd lng="cs">Giordano Bruno</trd>
  <trd lng="sk">Giordano Bruno</trd>
  <trd lng="uk">&#x411;&#x440;&#x443;&#x43D;&#x43E;&#xA0;(&#x456;&#x43C;&#x2019;&#x44F;)</trd>
</drv>
</art>

