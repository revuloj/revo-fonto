<drv mrk="suker.0ajxo">
  <kap><ofc>*</ofc><tld/>a&jcirc;o</kap>
  <snc mrk="suker.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="S"/>ita franda&jcirc;o:
      <ekz>
        sur la tablo staris diversaj <tld/>a&jcirc;oj
        <fnt><bib>F</bib>, <lok>&FE; 35</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;iu el ili anta&ubreve;tenis al li plej &ccirc;arman <tld/>a&jcirc;on, kiun nur povas vendi la
        kukistino
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i prenis rozan <tld/>a&jcirc;on <klr>[&#8230;]</klr> kraketigante &gcirc;in en siaj blankaj dentoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi havas 80 jarojn kaj spertis kiel, &ccirc;e ni, la boligitaj
        <tld/>a&jcirc;oj, biskvitoj, ktp. ofte estis sen volva&jcirc;oj kaj liveritaj al
        a&ccirc;etantoj en simplaj papersakoj, kiam mi estis infano
        <fnt>
          <aut>Henry D. Palmer</aut>,
          <vrk>Papera mal&scirc;paro</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2001a, numero 12a, p. 4a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="bombon.0o">bombono</ref>,
      <ref cel="sxpin.suker0ajxo">suker&scirc;pina&jcirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_d;&c_y;&c_t;&c_a;&c_r;&c_s;&c_k;&c_ib;
  &c_v;&c_y;&c_r;&c_a;&c_b;</trd>
  <trdgrp lng="ca">
