<drv mrk="suker.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Dol&ccirc;igi per <tld/>o:
      <ekz>
        tro <tld/>ita turka kafo
        <fnt>
          <bib>CKv</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi plenigos la tasojn kaj ilin <tld/>os mem, &ccirc;ar mi sola konas la gustojn de
        miaj kunman&gcirc;antoj
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i decidis baki ian delikata&jcirc;on, al kiu la&ubreve; nia hungara kutimo oni bezonas aligi
        pistitan kaj <tld/>itan papavon
        <fnt>
          <bib>KKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni man&gcirc;as makaroniojn kun <klr>[&#8230;]</klr> kakaopulvoro, <tld/>itaj muelitaj
        juglandoj
        <fnt>
          <bib>Kon11</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu viro ricevis malgrandan ladskatolon kun kondensita kaj <tld/>ita lakto
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_l;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol; <klr>(&c_c;&c_u;&c_k;&c_r;&c_a;&c_m;)</klr></trd>,
    <trd>&c_d;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; &c_c;&c_u;&c_k;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="cs">sladit cukrem</trd>
  <trd lng="de">zuckern</trd>
  <trd lng="es">azucarar</trd>
  <trdgrp lng="fr">
    <trd>sucrer</trd>
  </trdgrp>
  <trd lng="hu">cukroz</trd>
  <trd lng="id">meng<ind>gula</ind>i</trd>
  <trd lng="io">sukrizar</trd>
  <trd lng="it">zuccherare</trd>
  <trd lng="nl">zoeten</trd>
  <trd lng="ru">&c_k;&c_l;&c_a;&c_s;&c_t;&c_mol; <ind>&c_s;&c_a;&c_h;&c_a;&c_r;</ind><klr> (&c_v;&c_o;
  &c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>
  <trd lng="sk">sladi&tcaron; cukrom</trd>
  <trd lng="sv">sockra</trd>
</drv>

<drv mrk="suker.0ajxo">
  <kap><ofc>*</ofc><tld/>a&jcirc;o</kap>
  <snc mrk="suker.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="S"/>ita franda&jcirc;o:
      <ekz>
        sur la tablo staris diversaj <tld/>a&jcirc;oj
        <fnt><bib>F</bib>, <lok>&FE; 35</lok></fnt>;
      </ekz>
      <ekz>
        mi havas 80 jarojn kaj spertis kiel, &ccirc;e ni, la boligitaj
        <tld/>a&jcirc;oj, biskvitoj, ktp. ofte estis sen volva&jcirc;oj kaj liveritaj al
        a&ccirc;etantoj en simplaj papersakoj, kiam mi estis infano
        <fnt>
          <aut>Henry D. Palmer</aut>,
          <vrk>Papera mal&scirc;paro</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2001a, numero 12a, p. 4a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="bombon.0o">bombono</ref>,
      <ref cel="sxpin.suker0ajxo">suker&scirc;pina&jcirc;o</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Konfekt</trd>,
    <trd>S&uuml;&szlig;igkeit</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>dulce</trd>,
    <trd>confite</trd>,
    <trd>chucher&iacute;a</trd>,
    <trd>golosina</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>sucrerie</trd>
  </trdgrp>
  <trd lng="hu">&eacute;dess&eacute;g</trd>
  <trd lng="io">sukrajo</trd>
  <trdgrp lng="it">
    <trd>zuccherino</trd>,
    <trd>dolce<klr> (sost.)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>suikerwaar</trd>,
    <trd>snoepgoed</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_a;&c_d;&c_o;&c_s;&c_t;&c_mol; <klr tip="amb">(&c_k;&c_u;&c_sh;&c_a;&c_n;&c_mol;&c_je;)</klr></trd>,
    <trd>&c_k;&c_o;&c_n;&c_d;&c_i;&c_t;&c_je;&c_r;&c_s;&c_k;&c_o;&c_je; &c_i;&c_z;&c_d;&c_je;&c_l;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="suker.0ajxejo">
  <kap><tld/>a&jcirc;ejo</kap>
  <snc mrk="suker.0ajxejo.KOM">
    <dif>
      Butiko, kie oni vendas <tld/>a&jcirc;ojn:
      <ekz>
        tiajn <klr>[bombonojn]</klr> ili anta&ubreve;e vidis nur en la
        <tld/>a&jcirc;ejoj
        <fnt><bib>InfanTorent2</bib>, <lok>unua parto, &ccirc;apitro
        1a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_u;&c_k;&c_je;&c_r;&c_n;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_d;&c_y;&c_t;&c_e;&c_r;&c_y;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_d;&c_y;&c_t;&c_a;&c_r;&c_s;&c_k;&c_a;&c_ja;</trd>
  </trdgrp>
  <trd lng="de">Konditorei</trd>
  <trdgrp lng="es">
    <trd>confiter&iacute;a</trd>,
    <trd>dulcer&iacute;a</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>confiserie</trd>
  </trdgrp>
  <trd lng="hu">cukr&aacute;szda</trd>
  <trd lng="io">sukrajerio</trd>
  <trd lng="it">confetteria</trd>
  <trd lng="nl">snoepwinkel</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_d;&c_i;&c_t;&c_je;&c_r;&c_s;&c_k;&c_a;&c_ja;</trd>
</drv>

<drv mrk="suker.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="suker.0ujo.KOMUNE">
    <dif>
      Vazo a&ubreve; alia ujo por konservi a&ubreve; prezenti <tld/>on:
      <ekz>
        estas <klr>[&#8230;]</klr> pulvora <tld/>o, en <tld/>ujoj
        <fnt><bib>Gerda</bib>, <lok>&ccirc;apitro 5a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_u;&c_k;&c_je;&c_r;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="cs">cuk&rcaron;enka</trd>
  <trd lng="en">sugar pot</trd>
  <trd lng="es">azucarero<klr> (recipiente)</klr></trd>
