<drv mrk="suker.0ajxo">
  <kap><ofc>*</ofc><tld/>a&jcirc;o</kap>
  <snc mrk="suker.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="S"/>ita franda&jcirc;o:
      <ekz>
        sur la tablo staris diversaj <tld/>a&jcirc;oj
        <fnt><bib>F</bib>, <lok>&FE; 35</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;iu el ili anta&ubreve;tenis al li plej &ccirc;arman <tld/>a&jcirc;on, kiun nur povas vendi la
        kukistino
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i prenis rozan <tld/>a&jcirc;on <klr>[&#8230;]</klr> kraketigante &gcirc;in en siaj blankaj dentoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi havas 80 jarojn kaj spertis kiel, &ccirc;e ni, la boligitaj
        <tld/>a&jcirc;oj, biskvitoj, ktp. ofte estis sen volva&jcirc;oj kaj liveritaj al
        a&ccirc;etantoj en simplaj papersakoj, kiam mi estis infano
        <fnt>
          <aut>Henry D. Palmer</aut>,
          <vrk>Papera mal&scirc;paro</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2001a, numero 12a, p. 4a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="bombon.0o">bombono</ref>,
      <ref cel="sxpin.suker0ajxo">suker&scirc;pina&jcirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_d;&c_y;&c_t;&c_a;&c_r;&c_s;&c_k;&c_ib; &c_v;&c_y;&c_r;&c_a;&c_b;</trd>
  <trdgrp lng="de">
    <trd>Konfekt</trd>,
    <trd>S&uuml;&szlig;igkeit</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>dulce</trd>,
    <trd>confite</trd>,
    <trd>chucher&iacute;a</trd>,
    <trd>golosina</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>sucrerie</trd>
  </trdgrp>
  <trd lng="hu">&eacute;dess&eacute;g</trd>
  <trd lng="io">sukrajo</trd>
  <trdgrp lng="it">
    <trd>zuccherino</trd>,
    <trd>dolce<klr> (sost.)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#30722;&#x7CD6;&#x83D3;&#x5B50;  <pr>&#12373;&#12392;&#12358;&#x304C;&#12375;</pr></trd>
  <trdgrp lng="nl">
    <trd>suikerwaar</trd>,
    <trd>snoepgoed</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>s&lstroke;odycz</trd>,
    <trd>co&sacute; <ind>s&lstroke;odkiego</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_a;&c_d;&c_o;&c_s;&c_t;&c_mol; <klr tip="amb">(&c_k;&c_u;&c_sh;&c_a;&c_n;&c_mol;&c_je;)</klr></trd>,
    <trd>&c_k;&c_o;&c_n;&c_d;&c_i;&c_t;&c_je;&c_r;&c_s;&c_k;&c_o;&c_je; &c_i;&c_z;&c_d;&c_je;&c_l;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&#x43D;&c_d;&c_i;&c_t;&c_je;&c_r;&c_s;&#x44C;&#x43A;&c_i;&c_j; &c_v;&c_i;&c_r;&c_ib;&c_b;</trd>,
    <trd>&c_s;&#x43E;&#x43B;&#x43E;&c_d;&#x43E;&c_shch;&c_ib;</trd>
  </trdgrp>
</drv>

<drv mrk="suker.0ajxejo">
  <kap><tld/>a&jcirc;ejo</kap>
  <snc mrk="suker.0ajxejo.KOM">
    <dif>
      Butiko, kie oni vendas <tld/>a&jcirc;ojn:
      <ekz>
        la vento alportis la sonojn de la <tld/>a&jcirc;ejo, li povis anka&ubreve; a&ubreve;di, kiel la
        urbanoj tie kantis &ccirc;e sia glaso da vino
        <fnt>
          <bib>Fab2</bib>
          <lok>Sonorilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiajn <klr>[bombonojn]</klr> ili anta&ubreve;e vidis nur en la
        <tld/>a&jcirc;ejoj
        <fnt><bib>InfanTorent2</bib>, <lok>unua parto, &ccirc;apitro
        1a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_u;&c_k;&c_je;&c_r;&c_n;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_d;&c_y;&c_t;&c_e;&c_r;&c_y;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_d;&c_y;&c_t;&c_a;&c_r;&c_s;&c_k;&c_a;&c_ja;</trd>
  </trdgrp>
  <trd lng="de">Konditorei</trd>
  <trdgrp lng="es">
    <trd>confiter&iacute;a</trd>,
    <trd>dulcer&iacute;a</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>confiserie</trd>
  </trdgrp>
  <trd lng="hu">cukr&aacute;szda</trd>
  <trd lng="io">sukrajerio</trd>
  <trd lng="it">confetteria</trd>
  <trd lng="ja">&#x83D3;&#x5B50;&#x5C4B;  <pr>&#x304B;&#12375;&#12420;</pr></trd>
  <trd lng="nl">snoepwinkel</trd>
  <trd lng="pl">cukiernia</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_d;&c_i;&c_t;&c_je;&c_r;&c_s;&c_k;&c_a;&c_ja;</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&#x43D;&c_d;&c_i;&c_t;&c_je;&c_r;&c_s;&#x44C;&#x43A;&c_a;</trd>,
    <trd>&c_c;&c_u;&#x43A;&c_je;&c_r;&#x43D;&#x44F;</trd>,
    <trd>&c_c;&c_u;&#x43A;&#x43E;&c_r;&#x43D;&#x44F;</trd>
  </trdgrp>
