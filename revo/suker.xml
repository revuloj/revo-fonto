<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>suker</rad>/o
</kap>
<drv mrk="suker.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Kristaleca blanka dol&ccirc;a substanco, konsistanta el karbono, hidrogeno
      kaj oksigeno, kaj ekstraktata el diversaj vegeta&jcirc;oj, precipe el
      <tld/>kano kaj el beto:
      <ekz>
        la pli vaste konataj <tld/>oj, glukozo kaj fruktozo, havas
        en &ccirc;iu molekulo ses karbon-atomojn<fnt><aut>Ste&ccirc;jo Norvell</aut>:
        <vrk>Riboj</vrk>,
        <bib>Monato</bib><lok>jaro 2000a, numero 11a, p. 16a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_u;&c_k;&c_a;&c_r;</trd>
  <trd lng="cs">cukr</trd>
  <trd lng="de">Zucker</trd>
  <trd lng="en">sugar</trd>
  <trd lng="fr">sucre</trd>
  <trd lng="hu">cukor</trd>  
  <trd lng="nl">suiker</trd>
  <trd lng="ru">&c_s;&c_a;&c_h;&c_a;&c_r;</trd>
  <trd lng="sv">socker</trd>
  <trd lng="tr">&scedil;eker</trd>
</drv>

<drv mrk="suker.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Dol&ccirc;igi per <tld/>o.
    </dif>
  </snc>
  <trd lng="de">zuckern</trd>
  <trdgrp lng="fr">
    <trd>sucrer</trd>
  </trdgrp>
  <trd lng="hu">cukroz</trd>  
  <trd lng="nl">zoeten</trd>
  <trd lng="sv">sockra</trd>
  <trd lng="ru">&c_k;&c_l;&c_a;&c_s;&c_t;&c_mol; <ind>&c_s;&c_a;&c_h;&c_a;&c_r;</ind><klr> (&c_v;&c_o; &c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>
</drv>

<drv mrk="suker.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      <tld lit="S"/>ita franda&jcirc;o:
      <ekz>
        Mi havas 80 jarojn kaj spertis kiel, &ccirc;e ni, la bolitaj <tld/>a&jcirc;oj,
        biskvitoj, ktp. ofte estis sen volva&jcirc;oj kaj liveritaj al a&ccirc;etantoj
        en simplaj papersakoj, kiam mi estis infano<fnt><aut>Henry D.
        Palmer</aut>: <vrk>Papera mal&scirc;paro</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 12a, p. 4a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">S&uuml;&szlig;igkeit</trd>
  <trdgrp lng="fr">
    <trd>sucrerie</trd>
  </trdgrp>
  <trd lng="hu">&eacute;dess&eacute;g</trd>  
  <trdgrp lng="nl">
    <trd>suikerwaar</trd>,
    <trd>snoepgoed</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_a;&c_d;&c_o;&c_s;&c_t;&c_mol; <klr tip="amb">(&c_k;&c_u;&c_sh;&c_a;&c_n;&c_mol;&c_je;)</klr></trd>,
    <trd>&c_k;&c_o;&c_n;&c_d;&c_i;&c_t;&c_je;&c_r;&c_s;&c_k;&c_o;&c_je; &c_i;&c_z;&c_d;&c_je;&c_l;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="suker.0ajxejo">
  <kap><tld/>a&jcirc;ejo</kap>
  <snc>
    <dif>
      Butiko, kie oni vendas <tld/>a&jcirc;ojn.
    </dif>
  </snc>
  <trd lng="de">Konditorei</trd>
  <trdgrp lng="fr">
    <trd>confiserie</trd>
  </trdgrp>
  <trd lng="hu">cukr&aacute;szda</trd>  
  <trd lng="nl">snoepwinkel</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_d;&c_i;&c_t;&c_je;&c_r;&c_s;&c_k;&c_a;&c_ja;</trd>
</drv>

<drv mrk="suker.lakto0o">
  <kap>lakto<tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="laktoz.0o">Laktozo</ref>.
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>tejcukor</trd>,
    <trd>lakt&oacute;z</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_l;&c_o;&c_ch;&c_n;&c_y;&c_j; &c_s;&c_a;&c_h;&c_a;&c_r;</trd>,
    <trd>&c_l;&c_a;&c_k;&c_t;&c_o;&c_z;&c_a;</trd>
  </trdgrp>
</drv>
</art>
