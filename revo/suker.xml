<drv mrk="suker.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Dol&ccirc;igi per <tld/>o:
      <ekz>
        tro <tld/>ita turka kafo
        <fnt>
          <bib>CKv</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi plenigos la tasojn kaj ilin <tld/>os mem, &ccirc;ar mi sola konas la gustojn de
        miaj kunman&gcirc;antoj
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i decidis baki ian delikata&jcirc;on, al kiu la&ubreve; nia hungara kutimo oni bezonas aligi
        pistitan kaj <tld/>itan papavon
        <fnt>
          <bib>KKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni man&gcirc;as makaroniojn kun <klr>[&#8230;]</klr> kakaopulvoro, <tld/>itaj muelitaj
        juglandoj
        <fnt>
          <bib>Kon11</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu viro ricevis malgrandan ladskatolon kun kondensita kaj <tld/>ita lakto
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_l;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol; <klr>(&c_c;&c_u;&c_k;&c_r;&c_a;&c_m;)</klr></trd>,
    <trd>&c_d;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; &c_c;&c_u;&c_k;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="cs">sladit cukrem</trd>
  <trd lng="de">zuckern</trd>
  <trd lng="es">azucarar</trd>
  <trdgrp lng="fr">
    <trd>sucrer</trd>
  </trdgrp>
  <trd lng="hu">cukroz</trd>
  <trd lng="id">meng<ind>gula</ind>i</trd>
  <trd lng="io">sukrizar</trd>
  <trd lng="it">zuccherare</trd>
  <trd lng="ja">&#30722;&#x7CD6;&#12434;&#20837;&#x308C;&#x308B;  <pr>&#12373;&#12392;&#12358;&#12434;&#12356;&#x308C;&#x308B;</pr></trd>
  <trd lng="nl">zoeten</trd>
  <trdgrp lng="pl">
    <trd>s&lstroke;odzi&cacute;</trd>,
    <trd>cukrowa&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_l;&c_a;&c_s;&c_t;&c_mol; <ind>&c_s;&c_a;&c_h;&c_a;&c_r;</ind><klr> (&c_v;&c_o;
  &c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>
  <trd lng="sk">sladi&tcaron; cukrom</trd>
  <trd lng="sv">sockra</trd>
  <trdgrp lng="uk">
