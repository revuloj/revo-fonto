    <trd>&c_c;&c_u;&#x43A;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_d;&#x43E;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &c_c;&c_u;&#x43A;&#x43E;&c_r;</trd>,
    <trd>&#x43F;&#x43E;&c_s;&c_i;&#x43F;&c_a;&c_t;&c_i; &c_c;&c_u;&#x43A;&c_r;&#x43E;&#x43C;</trd>
