    <trd>llepolia</trd>,
    <trd>dol&ccedil;os</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Konfekt</trd>,
    <trd>S&uuml;&szlig;igkeit</trd>
  </trdgrp>
  <trdgrp lng="es">
