<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>suker</rad>/o
</kap>
<drv mrk="suker.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Kristaleca blanka dol&ccirc;a substanco, konsistanta el karbono, hidrogeno
      kaj oksigeno, kaj ekstraktata el diversaj vegeta&jcirc;oj, precipe el
      <tld/>kano kaj el beto.
    </dif>
  </snc>
  <trd lng="cs">cukr</trd>
  <trd lng="de">Zucker</trd>
  <trd lng="en">sugar</trd>
  <trd lng="fr">sucre</trd>
  <trd lng="nl">suiker</trd>
  <trd lng="sv">socker</trd>
  <trd lng="tr">&scedil;eker</trd>
</drv>

<drv mrk="suker.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Dol&ccirc;igi per <tld/>o.
    </dif>
  </snc>
  <trd lng="de">zuckern</trd>
  <trdgrp lng="fr">
    <trd>sucrer</trd>
  </trdgrp>
  <trd lng="nl">zoeten</trd>
  <trd lng="sv">sockra</trd>
</drv>

<drv mrk="suker.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc>
    <dif>
      <tld/>ita franda&jcirc;o.
    </dif>
  </snc>
  <trd lng="de">S&uuml;&szlig;igkeit</trd>
  <trdgrp lng="fr">
    <trd>sucrerie</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>suikerwaar</trd>,
    <trd>snoepgoed</trd>
  </trdgrp>
</drv>

<drv mrk="suker.0ajxejo">
  <kap>
    <tld/>a&jcirc;ejo
  </kap>
  <snc>
    <dif>
      Butiko, kie oni vendas <tld/>a&jcirc;ojn.
    </dif>
  </snc>
  <trd lng="de">Konditorei</trd>
  <trdgrp lng="fr">
    <trd>confiserie</trd>
  </trdgrp>
  <trd lng="nl">snoepwinkel</trd>
</drv>

<drv mrk="suker.lakto0o">
  <kap>
    lakto<tld/>o
  </kap>
  <snc>
    <dif>
      <ref tip="dif" cel="laktoz.0o">Laktozo</ref>.
    </dif>
  </snc>
</drv>
</art>
