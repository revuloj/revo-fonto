<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kontrakt</rad>/i
</kap>
<drv mrk="kontra3.0i">
  <kap>
    <ofc>*</ofc><tld/>i
  </kap>
  <uzo tip="fak">JUR</uzo>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <gra><vspec>x</vspec></gra>
    <dif>
      Inter&scirc;an&gcirc;i la&ubreve;le&gcirc;ajn skribitajn promesojn por komuna entrepreno:
      <ekz>
        <tld/>i liveradon por la armeo<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i pacon, pri paco, akordon, traktaton, vendakton;
      </ekz>
      <ekz>
        la altaj <tld/>antoj
        <klr>(la partoprenantaj registaroj)</klr>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>einen <ind>Vertrag schlie&szlig;en</ind></trd>,
     <trd>vertraglich vereinbaren</trd>,
     <trd>kontrahieren [ark]</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>engage</trd>,
    <trd>enter into a <ind>contract</ind> with</trd>
  </trdgrp>
  <trd lng="nl">contracteren</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_k;&c_l;&c_ju;&c_ch;&c_i;&c_t;&c_mol; &c_d;&c_o;&c_g;&c_o;&c_v;&c_o;&c_r;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_l;&c_ju;&c_ch;&c_i;&c_t;&c_mol; &c_k;&c_o;&c_n;&c_t;&c_r;&c_a;&c_k;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="kontra3.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">JUR</uzo>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      La&ubreve;le&gcirc;a skribita inter&scirc;an&gcirc;o de promeso, interpromeso, konvencio:
      <ekz>
        <tld/>o pri vendo, farmoluo;
      </ekz>
      <ekz>
        <tld/>o pri edzi&gcirc;o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Vertrag</trd>,
     <trd>Kontrakt [spec]</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>binding <ind>agreement</ind></trd>,
    <trd>contract</trd>,
    <trd>covenant</trd>,
    <trd>deal</trd>,
    <trd>legal agreement</trd>,
    <trd>pact</trd>
  </trdgrp>
  <trd lng="nl">contract</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_g;&c_o;&c_v;&c_o;&c_r;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_t;&c_r;&c_a;&c_k;&c_t;</trd>
  </trdgrp>
</drv>
</art>
