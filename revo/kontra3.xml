    <trd>kontrahovat</trd>,
    <trd>sepsat smlouvu</trd>,
    <trd>uzav&rcaron;&iacute;t smlouvu</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><ofc>*</ofc>einen <ind>Vertrag abschlie&szlig;en</ind></trd>,
    <trd>vertraglich vereinbaren</trd>,
    <trd>kontrahieren <klr>(kaufm.)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
