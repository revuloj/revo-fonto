<kap>
  <rad>emetik</rad>/o
<fnt><bib>PVEF</bib></fnt></kap>
<drv mrk="emetik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Antimona a&ubreve; kalia tartrato, kies solva&jcirc;o estas uzata por
      vomigado:
      <ekz>
        vomigi pacienton per <tld/>o
        <fnt>
          <lok><url
          ref="http://corpora.uni-leipzig.de/de/res?corpusId=epo_mixed_2012&amp;word=emetiko">esperanto.cri.cn</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_n;&c_ib;&c_t;&c_n;&c_y; <ind>&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;</ind></trd>
  <trd lng="ca">em&egrave;tic</trd>
  <trdgrp lng="cs">
    <trd>d&aacute;vidlo</trd>,
    <trd>vrhavka</trd>,
    <trd>v&aacute;penn&yacute; nebo antimonov&yacute; tartr&aacute;t <klr>(s&uring;l kyseliny vinn&eacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Emetikum</trd>,
     <trd>Brechmittel</trd>
  </trdgrp>
  <trd lng="es">em&eacute;tico</trd>
  <trd lng="fr">&eacute;m&eacute;tique</trd>
  <trdgrp lng="hu">
    <trd>h&aacute;nytat&oacute; bork&odblac;</trd>,
    <trd>h&aacute;nytat&oacute;szer</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#21520;&#21092;  <pr>&#12392;&#12374;&#12356;</pr></trd>,
    <trd>&#x50AC;&#21520;&#x85AC;  <pr>&#12373;&#12356;&#12392;&#12420;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="pl">emetyk</trd>
  <trdgrp lng="pt">
    <trd>em&eacute;tico</trd>,
    <trd>vomit&oacute;rio</trd>,
    <trd>vomitativo</trd>
  </trdgrp>
  <trd lng="sk">d&aacute;viv&yacute; v&iacute;nny kame&ncaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_je;&#x43C;&c_je;&c_t;&c_i;&#x43A;</trd>,
    <trd>&c_b;&#x43B;&#x44E;&c_v;&#x43E;&c_t;&#x43D;&c_je; (&c_u; &c_z;&#x43D;&c_a;&c_ch;.&c_ib;&#x43C;.)</trd>
  </trdgrp>
</drv>
</art>
