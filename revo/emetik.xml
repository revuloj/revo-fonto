<art mrk="$Id: emetik.xml,v 1.13 2022/03/17 13:47:18 revo Exp $">
<kap>
  <rad>emetik</rad>/o
</kap>
<drv mrk="emetik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Antimona a&ubreve; kalia tartrato, kies solva&jcirc;o estas uzata por
      vomigado:
      <ekz>
        vomigi pacienton per <tld/>o
        <fnt>
          <lok><url ref="http://corpora.uni-leipzig.de/de/res?corpusId=epo_mixed_2012&amp;word=emetiko">esperanto.cri.cn</url></lok>
        </fnt>.
      </ekz>  
    </dif>
  </snc>
  <trd lng="ca">em&egrave;tic</trd>
  <trdgrp lng="cs">
    <trd>d&aacute;vidlo</trd>,
    <trd>vrhavka</trd>,
    <trd>v&aacute;penn&yacute; nebo antimonov&yacute; tartr&aacute;t <klr>(s&uring;l kyseliny vinn&eacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Emetikum</trd>,
     <trd>Brechmittel</trd>
  </trdgrp>
  <trd lng="es">em&eacute;tico</trd>
  <trd lng="fr">&eacute;m&eacute;tique</trd>
  <trdgrp lng="hu">
    <trd>h&aacute;nytat&oacute; bork&odblac;</trd>,
    <trd>h&aacute;nytat&oacute;szer</trd>
  </trdgrp>
  <trd lng="pl">emetyk</trd>
  <trdgrp lng="pt">
    <trd>em&eacute;tico</trd>,
    <trd>vomit&oacute;rio</trd>,
    <trd>vomitativo</trd>
  </trdgrp>
  <trd lng="sk">d&aacute;viv&yacute; v&iacute;nny kame&ncaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x435;&#x43C;&#x435;&#x442;&#x438;&#x43A;</trd>,
    <trd>&#x431;&#x43B;&#x44E;&#x432;&#x43E;&#x442;&#x43D;&#x435; (&#x443; &#x437;&#x43D;&#x430;&#x447;.&#x456;&#x43C;.)</trd>
  </trdgrp>
</drv>
</art>
