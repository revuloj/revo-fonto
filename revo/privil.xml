<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>privilegi</rad>/o
</kap>
<drv mrk="privil.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Aparta rajto a&ubreve; favoro, koncesiita al iu ekster la komuna rajto:
      <ekz>
        la <tld/>oj de la klerikaro, nobelaro, re&gcirc;oj;
      </ekz>
      <ekz>
        la <ind><tld/>a</ind>j klasoj;
        <trd lng="be">&c_p;&c_r;&c_y;&c_v;&c_ib;&c_l;&c_je;&c_ja;&c_v;&c_a;&c_n;&c_y;</trd>
        <trd lng="hu">kiv&aacute;lts&aacute;gos</trd>        
        <trd lng="ru">&c_p;&c_r;&c_i;&c_v;&c_i;&c_l;&c_je;&c_g;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
      </ekz>
      <ekz>
        la <tld/>uloj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_v;&c_ib;&c_l;&c_je;&c_j;</trd>
  <trdgrp lng="de">
    <trd>Privileg</trd>,
    <trd>Vorrecht</trd>,
    <trd>Sonderrecht</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>el&odblac;jog</trd>,
    <trd>kiv&aacute;lts&aacute;g</trd>,
    <trd>privil&eacute;gium</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>privilege</trd>,
    <trd>voorrecht</trd>
  </trdgrp>
  <trd lng="pt">privil&eacute;gio</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_v;&c_i;&c_l;&c_je;&c_g;&c_i;&c_ja;</trd>
</drv>

<drv mrk="privil.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Koncesii <tld/>on al iu:
      <ekz>
        &gcirc;is kiam vi ju&gcirc;os maljuste kaj <tld/>os la
        malvirtulojn?<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="monopo.0o">monopolo</ref>,
      <ref cel="prerog.0o">prerogativo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_c;&c_mol; <ind>&c_p;&c_r;&c_y;&c_v;&c_ib;&c_l;&c_je;&c_j;</ind></trd>
  <trdgrp lng="de">
    <trd>privilegieren</trd>,
    <trd>ein <ind>Sonderrecht</ind> einr&auml;umen</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kiv&aacute;lts&aacute;gban r&eacute;szes&iacute;t</trd>,
    <trd>privilegiz&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">bevoorrechten</trd>
  <trd lng="ru">&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol; <ind>&c_p;&c_r;&c_i;&c_v;&c_i;&c_l;&c_je;&c_g;&c_i;&c_i;</ind></trd>
</drv>
</art>
