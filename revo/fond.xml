<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fond</rad>/i
</kap>
<drv mrk="fond.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1">
    <dif>
      Krei, starigi organizon:
      <ekz>
        <tld/>i institucion, societon<fnt>B</fnt>, foiron, premion,
        urbon, familion;
      </ekz>
      <ekz>
        kiel <tld/>into de Esp-o mi ricevis multajn gratulajn
        leterojn<fnt>Z</fnt>;
      </ekz>
      <ekz><tld/>o de laborista klubo.</ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_o;&c_s;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_u;&c_ch;&c_r;&c_je;&c_d;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Motivi, bazi:
      <ekz>
        tiu opinio estas <tld/>ita sur nesciado de la historio<fnt>Z</fnt>;
      </ekz>
      <ekz>
        sensenca&jcirc;o <tld/>ita sur nenio<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fundam.0o">fundamento</ref>.
    </refgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_o;&c_s;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_m;&c_o;&c_t;&c_i;&c_v;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="en">establish</trd>
</drv>
<drv mrk="fond.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Establejo kreita por speciala, ordinare socia, celo,
      k provizita per sufi&ccirc;a kapitalo a&ubreve; rento por povi da&ubreve;re funkciadi. 
    </dif>
  </snc>
  <trd lng="ru">&c_f;&c_o;&c_n;&c_d;</trd>
</drv>
<drv mrk="fond.sen0a">
  <kap>sen<tld/>a</kap>
  <snc>
    <dif>
      Senbaza,  senmotiva:
      <ekz>sen<tld/>a postulo, gusto<fnt>Z</fnt>.</ekz>  
    </dif>
  </snc>
</drv>
</art>
