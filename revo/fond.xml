<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fond</rad>/i
</kap>
<drv mrk="fond.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Krei, starigi organizon:
      <ekz>
        <tld/>i institucion, societon<fnt>B</fnt>, foiron, premion,
        urbon, familion;
      </ekz>
      <ekz>
        kiel <ind><tld/>into</ind> de E-o mi ricevis multajn gratulajn
        leterojn<fnt>Z</fnt>;
        <trdgrp lng="fr">
          <trd>fondateur</trd>,
          <trd>p&egrave;re<klr tip="ind"> (fondateur)</klr></trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind><tld/>o</ind> de laborista klubo.
        <trdgrp lng="fr">
          <trd>fondation<klr tip="ind"> (instauration)</klr></trd>,
          <trd>instauration</trd>
        </trdgrp>
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_o;&c_s;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_u;&c_ch;&c_r;&c_je;&c_d;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Motivi, bazi:
      <ekz>
        tiu opinio estas <tld/>ita sur nesciado de la historio<fnt>Z</fnt>;
      </ekz>
      <ekz>
        sensenca&jcirc;o <tld/>ita sur nenio<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_o;&c_s;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_m;&c_o;&c_t;&c_i;&c_v;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <refgrp tip="vid">
    <ref cel="baz.0i">bazi</ref>,
    <ref cel="kre.0i">krei</ref>,
    <ref cel="fundam.0i">fundamenti</ref>.
  </refgrp>
  <trd lng="de">gr&uuml;nden</trd>
  <trd lng="en">establish</trd>
  <trdgrp lng="fr">
    <trd>fonder<klr tip="ind"> (ts sauf servir de base)</klr></trd>,
    <trd>asseoir<klr tip="ind"> (fonder)</klr></trd>,
    <trd>&eacute;tablir</trd>,
    <trd>&eacute;difier</trd>,
    <trd>instaurer</trd>
  </trdgrp>
</drv>
<drv mrk="fond.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Establejo kreita por speciala, ordinare socia, celo,
      kaj provizita per sufi&ccirc;a kapitalo a&ubreve; rento por povi da&ubreve;re funkciadi.
    </dif>
    <refgrp tip="vid">
      Ne konfuzu kun:
      <ref cel="fondus.0o">fonduso</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Stiftung</trd>
  <trd lng="fr">fondation<klr tip="ind"> (institution)</klr></trd>
  <trd lng="ru">&c_f;&c_o;&c_n;&c_d;</trd>
</drv>
<drv mrk="fond.sen0a">
  <kap>sen<tld/>a</kap>
  <snc>
    <dif>
      Senbaza,  senmotiva:
      <ekz>
        sen<tld/>a postulo, gusto<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">unbegr&uuml;ndet</trd>
  <trd lng="fr">infond&eacute;</trd>
</drv>
</art>
