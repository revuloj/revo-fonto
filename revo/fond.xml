    <trd>&#x6839;&#x62E0;&#x3092;&#x7F6E;&#x304F;  <pr>&#x3053;&#x3093;&#x304D;&#x3087;&#x3092;&#x304A;&#x304F;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kon&scaron;tituova&tcaron;</trd>,
    <trd>zalo&zcaron;i&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>zalo&zcaron;it</trd>,
    <trd>z&rcaron;&iacute;dit</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_s;&c_n;&c_o;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_t;&c_v;&c_o;&c_r;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_o;&c_ch;&c_a;&c_t;&c_k;&c_o;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_b;&c_gu;&c_r;&c_u;&c_n;&c_t;&c_o;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
