<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fond</rad>/i
</kap>
<drv mrk="fond.0i">
   <kap><ofc>*</ofc><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc mrk="fond.0i.starigi">
     <dif>
       Krei, starigi organizon:
       <ekz>
         <tld/>i institucion, societon<fnt>B</fnt>, foiron, premion,
         urbon, familion;
       </ekz>
       <ekz>
         kiel <ind><tld/>into</ind> de Esperanto mi ricevis multajn gratulajn
         leterojn<fnt>Z</fnt>;
         <trd lng="be">&c_z;&c_a;&c_s;&c_n;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
         <trdgrp lng="fr">
           <trd>fondateur</trd>,
           <trd>p&egrave;re<klr tip="ind"> (fondateur)</klr></trd>
         </trdgrp>
         <trdgrp lng="hu">
           <trd>alap&iacute;t&oacute;</trd>,
           <trd>alap&iacute;tv&aacute;nytev&odblac;</trd>
         </trdgrp>
         <trd lng="nl">stichter</trd>
       </ekz>
       <ekz>
         <ind><tld/>o</ind> de laborista klubo.
         <trdgrp lng="fr">
           <trd>fondation<klr tip="ind"> (instauration)</klr></trd>,
           <trd>instauration</trd>
         </trdgrp>
       <trd lng="hu">alap&iacute;t&aacute;s</trd>
       <trd lng="nl">oprichting</trd>
       </ekz>
     </dif>
     <trd lng="be">&c_z;&c_a;&c_s;&c_n;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
     <trd lng="es">fundar</trd>
     <trd lng="hu">alap&iacute;t</trd>
     <trd lng="nl">stichten</trd>
     <trdgrp lng="ru">
       <trd>&c_o;&c_s;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
       <trd>&c_u;&c_ch;&c_r;&c_je;&c_d;&c_i;&c_t;&c_mol;</trd>
     </trdgrp>
   </snc>
   <snc mrk="fond.0i.FIG">
     <uzo tip="stl">FIG</uzo>
     <dif>
       Motivi, bazi:
       <ekz>
         tiu opinio estas <tld/>ita sur nesciado de la historio<fnt>Z</fnt>;
       </ekz>
       <ekz>
         sensenca&jcirc;o <tld/>ita sur nenio<fnt>Z</fnt>.
       </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>&c_a;&c_b;&c_g;&c_r;&c_u;&c_n;&c_t;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>,
       <trd>&c_m;&c_a;&c_t;&c_y;&c_v;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
     </trdgrp>
     <trd lng="es">fundamentar</trd>
     <trd lng="hu">alapoz<klr tip="ind"> (&aacute;tv.)</klr></trd>
     <trd lng="nl">z. <ind>baseren</ind></trd>
     <trdgrp lng="ru">
       <trd>&c_o;&c_b;&c_o;&c_s;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
       <trd>&c_m;&c_o;&c_t;&c_i;&c_v;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
     </trdgrp>
   </snc>
   <refgrp tip="vid">
     <ref cel="baz.0i">bazi</ref>,
     <ref cel="fundam.0i">fundamenti</ref>,
     <ref cel="kre.0i">krei</ref>
   </refgrp>
   <trd lng="de">gr&uuml;nden</trd>
   <trd lng="en">establish</trd>
   <trdgrp lng="fr">
     <trd>fonder<klr tip="ind"> (ts sauf servir de base)</klr></trd>,
     <trd>asseoir<klr tip="ind"> (fonder)</klr></trd>,
     <trd>&eacute;tablir</trd>,
     <trd>&eacute;difier</trd>,
     <trd>instaurer</trd>
   </trdgrp>
</drv>
<drv mrk="fond.0ajxo">
   <kap><tld/>a&jcirc;o</kap>
   <uzo tip="fak">EKON</uzo>
   <snc>
     <dif>
       Establejo kreita por speciala, ordinare socia, celo,
       kaj provizita per sufi&ccirc;a kapitalo a&ubreve; rento por povi da&ubreve;re funkciadi.
     </dif>
     <refgrp tip="vid">
       Ne konfuzu kun:
       <ref cel="fondus.0o">fonduso</ref>.
     </refgrp>
   </snc>
   <trd lng="be">&c_f;&c_o;&c_n;&c_d;</trd>
   <trd lng="de">Stiftung</trd>
   <trd lng="es">fundaci&oacute;n</trd>
   <trd lng="fr">fondation<klr tip="ind"> (institution)</klr></trd>
   <trd lng="hu">alap&iacute;tv&aacute;ny</trd>
   <trd lng="nl">stichting</trd>
   <trd lng="ru">&c_f;&c_o;&c_n;&c_d;</trd>
</drv>
<drv mrk="fond.sen0a">
   <kap>sen<tld/>a</kap>
   <snc>
     <dif>
       Senbaza, senmotiva:
       <ekz>
         sen<tld/>a postulo, gusto<fnt>Z</fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_b;&c_je;&c_s;&c_p;&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_n;&c_y;</trd>,
     <trd>&c_n;&c_je;&c_m;&c_a;&c_t;&c_y;&c_v;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
   </trdgrp>
   <trd lng="de">unbegr&uuml;ndet</trd>
   <trd lng="fr">infond&eacute;</trd>
   <trd lng="hu">alaptalan</trd>
   <trd lng="nl">ongegrond</trd>
   <trdgrp lng="ru">
     <trd>&c_b;&c_je;&c_z;&c_o;&c_s;&c_n;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
     <trd>&c_n;&c_je;&c_o;&c_b;&c_o;&c_s;&c_n;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
   </trdgrp>
</drv>
</art>
