<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: peonia.xml,v 1.12 2022/04/26 06:37:27 revo Exp $">
<kap>
  <rad>peoniac</rad>/oj
</kap>
<drv mrk="peonia.0oj">
  <kap><tld/>oj</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Paeoniaceae</trd>):
      <ekz>
        la <tld/>oj <klr>[&#8230;]</klr> nur konsistas el unu sola genro, tiu
        genro havas 32
        speciojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=511041"
              >Peoniacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="peoni.0o">peonio</ref>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_v;&c_o;&c_n;&c_je;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  <trd lng="cs">pivo&ncaron;kovit&eacute; <klr>(&ccaron;ele&dcaron;)</klr></trd>
  <trd lng="de">Pfingstrosengew&auml;chse</trd>
  <trdgrp lng="fr">
    <trd>Pa&eacute;oniac&eacute;es</trd>,
    <trd>P&eacute;oniac&eacute;es</trd>
  </trdgrp>
  <trd lng="hu">bazsar&oacute;zsaf&eacute;l&eacute;k</trd>
  <trd lng="nl">pioenroosfamilie</trd>
  <trd lng="pl">piwoniowate</trd>
  <trd lng="pt">peoni&aacute;ceas</trd>
  <trd lng="ro">Paeoniaceae</trd>
  <trd lng="ru">&c_p;&c_i;&c_o;&c_n;&c_o;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">piv&oacute;niovit&eacute;</trd>
</drv>
</art>
