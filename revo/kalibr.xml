<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kalibr.xml,v 1.26 2023/11/21 09:25:14 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>kalibr</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="kalibr.0o">
  <kap><tld/>o</kap>
  <snc mrk="kalibr.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="vid" cel="diamet.0o.cirklo">Diametro</ref> de objekto
      rigardata kiel mezuro de &gcirc;ia potenco a&ubreve; klaso:
      <ekz>
        malgrand-<tld/>a pafilo
        <fnt>
          <bib>SrT</bib>
          <lok>Libro IV.</lok>
        </fnt>.
      </ekz>
    </dif>
    <subsnc>
      <dif>
        Diametro interna de tubo:
        <ekz>
          <tld/>o de kanono, pafilo.
        </ekz>
      </dif>
    </subsnc>
    <subsnc>
      <dif>
        Ekstera diametro de cilindra a&ubreve; sfera korpo:
        <ekz>
          <tld/>o de kuglo, obuso.
        </ekz>
      </dif>
    </subsnc>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_ib;&c_b;&c_r;</trd>
  <trd lng="ca">calibre</trd>
  <trdgrp lng="cs">
    <trd>kalibr</trd>,
    <trd>sv&ecaron;tlost</trd>
  </trdgrp>
  <trd lng="de">Kaliber</trd>
  <trdgrp lng="en">
    <trd>caliber<klr> (US)</klr></trd>,
    <trd>calibre<klr> (BR)</klr></trd>,
  </trdgrp>
  <trd lng="es">calibre</trd>
  <trd lng="fr">calibre<klr tip="ind"> (mesure)</klr></trd>
  <trdgrp lng="hu">
    <trd>kaliber</trd>,
    <trd>bels&odblac; <ind>&aacute;tm&eacute;r&odblac;</ind></trd>,
    <trd>&ouml;b</trd>,
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#21475;&#24452;  <pr>&#12371;&#12358;&#12369;&#12356;</pr></trd>,
    <trd>&#20869;&#24452;  <pr>&#12394;&#12356;&#12369;&#12356;</pr></trd>,
    <trd>&#30452;&#24452;  <pr>&#12385;&#12423;&#12387;&#12369;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">kaliber</trd>
  <trd lng="pl">kaliber</trd>
  <trd lng="pt">calibre</trd>
  <trd lng="ro">calibru</trd>
  <trd lng="ru">&c_k;&c_a;&c_l;&c_i;&c_b;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>kaliber <klr>(svetlos&tcaron;, r&aacute;&zcaron;a, presn&eacute; meradlo)</klr></trd>,
    <trd>kaliber <klr>(svetlos&tcaron;| r&aacute;&zcaron;a| presn&eacute; meradlo)</klr></trd>
  </trdgrp>
  <trd lng="uk">&c_k;&c_a;&c_l;&c_ib;&c_b;&c_r;</trd>
</drv>

<drv mrk="kalibr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kalibr.0i.TEK">
    <dif>
      <ref tip="super" cel="kontro.0i.detale_esplori">Kontroli</ref> la <tld/>on de io:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        anta&ubreve; du semajnoj oni donis al &gcirc;i persikon, kaj la kerno tiom dolorigis &gcirc;ian
        posta&jcirc;on, ke la simio nun <tld/>as &ccirc;iun kernon
        <fnt>
          <bib>KrB</bib>
          <lok>Sa&gcirc;o abunda, sed ne profunda</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_m;&c_ja;&c_r;&c_a;&c_c;&c_mol; <ind>&c_k;&c_a;&c_l;&c_ib;&c_b;&c_r;</ind></trd>
  <trd lng="ca">calibrar (mesurar)</trd>
  <trdgrp lng="cs">
    <trd>kalibrovat</trd>,
    <trd>prov&aacute;d&ecaron;t kalibraci</trd>
  </trdgrp>
  <trd lng="de">kalibrieren</trd>
  <trd lng="en">calibrate</trd>
  <trd lng="es">calibrar</trd>
  <trd lng="fr">calibrer<klr tip="ind"> (mesurer le calibre)</klr></trd>
  <trd lng="hu">kalibr&aacute;l <klr tip="amb">(m&eacute;r)</klr></trd>
  <trd lng="ja">&#21475;&#24452;&#12539;&#30452;&#24452;&#12434;&#28204;&#23450;&#12377;&#12427;  <pr>&#12371;&#12358;&#12369;&#12356;&#12539;&#12385;&#12423;&#12387;&#12369;&#12356;&#12434;&#12381;&#12367;&#12390;&#12356;&#12377;&#12427;</pr></trd>
  <trd lng="nl">het <ind>kaliber</ind> bepalen</trd>
  <trd lng="pl">kalibrowa&cacute;</trd>
  <trd lng="ro">calibra</trd>
  <trd lng="ru">&c_v;&c_y;&c_v;&c_je;&c_r;&c_ja;&c_t;&c_mol; <ind>&c_k;&c_a;&c_l;&c_i;&c_b;&c_r;</ind></trd>
  <trdgrp lng="sk">
    <trd>kalibrova&tcaron;</trd>,
    <trd>mera&tcaron; kaliber</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_l;&c_ib;&c_b;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_m;&c_ib;&c_r;&c_ju;&c_v;&c_a;&c_t;&c_i; &c_k;&c_a;&c_l;&c_ib;&c_b;&c_r;</trd>,
    <trd>&c_v;&c_i;&c_v;&c_ib;&c_r;&c_ja;&c_t;&c_i; &c_k;&c_a;&c_l;&c_ib;&c_b;&c_r; (&c_v;&c_o;&c_g;&c_n;&c_je;&c_p;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_ji; &c_z;&c_b;&c_r;&c_o;&c_ji;)</trd>
  </trdgrp>
</drv>

<drv mrk="kalibr.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="kalibr.0igi.TEK">
    <dif>
      Fari, ke iu objekto havu ian precizan <tld/>on.
    </dif>
    <refgrp tip="vid">
      <ref cel="konfor.0igi">alkonformigi</ref>,
      <ref cel="norm.0hava">normigi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_ib;&c_b;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">calibrar <klr>(donar calibre)</klr></trd>
  <trdgrp lng="cs">
    <trd>kalibrovat</trd>,
    <trd>prov&aacute;d&ecaron;t kalibrov&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">auf ein Kaliber <ind>ausrichten</ind></trd>
  <trd lng="en">recalibrate</trd>
  <trd lng="es">calibrar</trd>
  <trd lng="fr">calibrer<klr tip="ind"> (mettre au calibre)</klr></trd>
  <trd lng="hu">kalibr&aacute;l <klr tip="amb">(m&eacute;retre szab)</klr></trd>
  <trd lng="ja">&#21475;&#24452;&#12539;&#30452;&#24452;&#12434;&#35519;&#31680;&#12377;&#12427;  <pr>&#12371;&#12358;&#12369;&#12356;&#12539;&#12385;&#12423;&#12387;&#12369;&#12356;&#12434;&#12385;&#12423;&#12358;&#12379;&#12388;&#12377;&#12427;</pr></trd>
  <trd lng="nl">kalibreren</trd>
  <trd lng="pl">wzorcowa&cacute;</trd>
  <trd lng="ro">etalona</trd>
  <trd lng="ru">&c_k;&c_a;&c_l;&c_i;&c_b;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">vytvori&tcaron; kaliber</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_l;&c_ib;&c_b;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &c_p;&c_o;&c_t;&c_r;&c_ib;&c_b;&c_n;&c_o;&c_g;&c_o; &c_k;&c_a;&c_l;&c_ib;&c_b;&c_r;&c_u;</trd>
  </trdgrp>
</drv>

<drv mrk="kalibr.0ilo">
  <kap><tld/>ilo <fnt><bib>PIV1</bib></fnt></kap>
  <snc mrk="kalibr.0ilo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Instrumento por kontroli <tld/>ojn:
      <ekz>
        li <klr>[&#8230;]</klr> pesis ilin per apoteka pesilo kaj kalkulis
        ilian grandon per armista <tld/>ilo
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>,
        <vrk>Cent
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/f/f5/GaugePlugSpecialGoNoGo.jpg/320px-GaugePlugSpecialGoNoGo.jpg"
      prm="CC BY-SA 3.0"
      ><tld/>ilo por bortruoj (maldekstra &gcirc;usta diametro, dekstre tro granda)
        <fnt>
          <aut>Glenn McKechnie, propra verko</aut>
          <lok><url ref="&WCW;/File:GaugePlugSpecialGoNoGo.jpg"
              >https://commons.wikimedia.org/wiki/File:GaugePlugSpecialGoNoGo.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="ca">calibrador</trd>
  <trdgrp lng="de">
    <trd>Grenzlehre</trd>,
    <trd>Kaliberlehre</trd>
  </trdgrp>
  <trd lng="en">go/no-go gauge</trd>
  <trdgrp lng="fr">
    <trd>calibreur<klr tip="ind"> (instrument)</klr></trd>,
    <trd>calibres &agrave; limites</trd>
  </trdgrp>
  <trd lng="ja">&#21475;&#24452;&#28204;&#23450;&#22120;  <pr>&#12371;&#12358;&#12369;&#12356;&#12381;&#12367;&#12390;&#12356;&#12358;&#12388;&#12431;</pr></trd>
  <trd lng="nl">kaliber<klr tip="ind"> (instrument)</klr></trd>
  <trd lng="pl">kaliber<klr>  (przyrz&aogonek;d)</klr></trd>
  <trd lng="ro">gauge<klr>  (instrumentul)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_l;&c_i;&c_b;&c_r;<klr tip="amb"> (&c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;)</klr></trd>,
    <trd>&c_k;&c_a;&c_l;&c_i;&c_b;&c_r;&c_a;&c_t;&c_o;&c_r;</trd>
  </trdgrp>
  <trd lng="uk">&c_k;&c_a;&c_l;&c_ib;&c_b;&c_r; (&c_ib;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;)</trd>
</drv>
</art>
