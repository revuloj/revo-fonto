<drv mrk="stutgart.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      &Ccirc;ef<ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
     de <ref tip="malprt" cel="vurtemberg.Baden0o">Baden-Virtembergo</ref> en Germanujo:
      <ekz>
        flagoj flirtis de turoj kaj tegmentoj de la re&gcirc;urbo <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Malnova pre&gcirc;eja sonorilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la 58-jara&gcirc;a instruistino laboras en lernejo en <tld/>o, Baden-Virtembergo
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2008/009472.php">Kaptukoj kaj kapdoloroj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        nun en <tld/>o multaj civitanoj postulas la eksigon de la regiona registaro kaj
        de la urbestro
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2010/010199.php">Volo de la popolo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_Sh;&c_t;&c_u;&c_t;&c_g;&c_a;&c_r;&c_t;</trd>
    <trd lng="de">Stuttgart</trd>
  </snc>
  <trd lng="ja">&#x30B7;&#x30E5;&#x30C8;&#x30A5;&#x30C3;&#x30C8;&#x30AC;&#x30EB;&#x30C8;</trd>
  <trd lng="pl">Stuttgart</trd>
  <trd lng="uk">&c_Sh;&c_t;&c_u;&c_t;&c_gu;&c_a;&c_r;&c_t;</trd>
