<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gavial</rad>/o
</kap>
<drv mrk="gavial.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="gavial.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Gavialis</trd>).
    </dif>
  </snc>
  <trd lng="ru">&c_g;&c_a;&c_v;&c_i;&c_a;&c_l;</trd>
</drv>
<drv mrk="gavial.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
       Familio el la ordo <ref tip="super" cel="krokod.0uloj">krokodiluloj</ref>
       (<trd lng="la">Gavialidae</trd>), kun tre mallar&gcirc;a bu&scirc;o,
       al kiu apartenas nur la genro <tld/>o.
     </dif>
  </snc>
  <trd lng="de">Gaviale</trd>
</drv>
</art>

