<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>monat</rad>/o
</kap>
<drv mrk="monat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;iu el la 12 partoj la&ubreve; kiuj oni dividas la jaron
    </dif>
    <refgrp tip="sub">
      <ref cel="januar.0o">januaro</ref>,
      <ref cel="februa.0o">februaro</ref>,
      <ref cel="mart.0o">marto</ref>,
      <ref cel="april.0o">aprilo</ref>,
      <ref cel="maj.0o">majo</ref>,
      <ref cel="juni.0o">junio</ref>,
      <ref cel="juli.0o">julio</ref>,
      <ref cel="auxgus.0o">a&ubreve;gusto</ref>,
      <ref cel="septem.0o">septembro</ref>,
      <ref cel="oktobr.0o">oktobro</ref>,
      <ref cel="novemb.0o">novembro</ref>,
      <ref cel="decemb.0o">decembro</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Da&ubreve;ro de dekduono de jaro:
      <ekz>
        voja&gcirc;o de du <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_s;&c_ja;&c_c;</trd>
  <trd lng="cs">m&ecaron;s&iacute;c</trd>
  <trd lng="de">Monat</trd>
  <trd lng="en">month</trd>
  <trd lng="fr">mois</trd>
  <trd lng="hu">h&oacute;nap</trd>
  <trd lng="nl">maand</trd>
  <trd lng="es">mes</trd>
  <trd lng="pl">miesi&aogonek;c</trd>
  <trd lng="ru">&c_m;&c_je;&c_s;&c_ja;&c_c;</trd>
  <trd lng="sk">mesiac <klr>(v kalend&aacute;ri)</klr></trd>
  <trd lng="sv">m&aring;nad</trd>
</drv>

<drv mrk="monat.0ajxo">
  <kap><tld/>a&jcirc;o<fnt>Z</fnt></kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="menstr.0o">Menstruo</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_je;&c_s;&c_ja;&c_ch;&c_n;&c_y;&c_ja;</trd>,
    <trd>&c_m;&c_e;&c_n;&c_s;&c_t;&c_r;&c_u;&c_a;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Monatsblutung</trd>,
    <trd>Regel</trd>,
    <trd>Menstruation</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>havi v&eacute;rz&eacute;s</trd>,
    <trd>menstru&aacute;ci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>maandstonden</trd>,
    <trd>regel</trd>,
    <trd>menstruatie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_s;&c_ja;&c_ch;&c_n;&c_y;&c_je;</trd>,
    <trd>&c_m;&c_je;&c_n;&c_s;&c_t;&c_r;&c_u;&c_a;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
