<drv mrk="monat.0a">
  <kap><tld/>a</kap>
  <snc mrk="monat.0a.KAL">
    <dif>
      Rilata al <tld/>o, precipe okazanta unufoje en <tld/>o:
      <ekz>
        <tld/>a fakrevuo
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/1993/A931103.php">Nova gaela numerala sistemo</url></vrk>
          <bib>Monato</bib>
          <lok>1993/11, p. 21</lok>
        </fnt>;
      </ekz>
      <ekz>
        du<tld/>a revuo
        <fnt>
          <aut>Boris Kolker</aut>
            <vrk><url ref="&Monato;/2000/006807.php">Fascina gazeto</url></vrk>
            <bib>Monato</bib>
            <lok>2000/03, p. 31</lok>
          </fnt>;
      </ekz>
      <ekz>
        la societo eldonas <tld/>an gazeton
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj,
        bagateloj</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>a nutrokvanto estis limigita je nur 13&#x202F;kg
        <fnt>
       <aut>Maryvonne kaj Bruno Robineau</aut>
          <vrk><url ref="&Monato;/2000/006831.php">La&ubreve; ritmo de la rizejoj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/02, p. 18</lok>
        </fnt>;
      </ekz>
      <ekz>
        por e&ccirc; mezbone vivi necesas <tld/>a enspezo de 188 dolaroj
        <fnt>
          <aut>LAST</aut>
            <vrk><url ref="&Monato;/2000/006804.php">Mizera statistiko</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 14</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_je;&c_s;&c_ja;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_sh;&c_t;&c_o;&c_m;&c_je;&c_s;&c_ja;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="br">miziek</trd>
  <trd lng="ca">mensual</trd>
  <trd lng="de">monatlich</trd>
  <trd lng="fr">mensuel</trd>
  <trdgrp lng="ja">
    <trd>&#x4E00;&#x30F6;&#x6708;&#x306E;  <pr>&#x3072;&#x3068;&#x304B;&#x3052;&#x3064;&#x306E;</pr></trd>,
    <trd>&#x6BCE;&#x6708;&#x306E;  <pr>&#x307E;&#x3044;&#x3064;&#x304D;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">maandelijks</trd>
  <trd lng="pl">miesi&eogonek;czny</trd>
  <trd lng="ro">lunare</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_s;&c_ja;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_je;&c_zh;&c_je;&c_m;&c_je;&c_s;&c_ja;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
