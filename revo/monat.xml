<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>monat</rad>/o
</kap>
<drv mrk="monat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      &Ccirc;iu el la 12 partoj la&ubreve; kiuj oni dividas la jaron
    </dif>
    <refgrp tip="sub">
      <ref cel="januar.0o">januaro</ref>,
      <ref cel="februa.0o">februaro</ref>,
      <ref cel="mart.0o">marto</ref>,
      <ref cel="april.0o">aprilo</ref>,
      <ref cel="maj.0o">majo</ref>,
      <ref cel="juni.0o">junio</ref>,
      <ref cel="juli.0o">julio</ref>,
      <ref cel="auxgus.0o">a&ubreve;gusto</ref>,
      <ref cel="septem.0o">septembro</ref>,
      <ref cel="oktobr.0o">oktobro</ref>,
      <ref cel="novemb.0o">novembro</ref>,
      <ref cel="decemb.0o">decembro</ref>. 
    </refgrp>
  </snc>
  <snc num="2">
    <dif>
      Da&ubreve;ro de dekduono da jaro:
      <ekz>
        voja&gcirc;o de du <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">m&ecaron;s&iacute;c</trd>
  <trd lng="de">Monat</trd>
  <trd lng="en">month</trd>
</drv>
<drv mrk="monat.0ajxo">
  <kap><tld/>a&jcirc;o<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Hemoragio, kiu okazas &ccirc;iu<tld/>e &ccirc;e la seksmatura virino. 
    </dif>
    <refgrp tip="vid">
      <ref cel="menstr.0o">menstruo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Monatsblutung</trd>,
    <trd>Regel</trd>,
    <trd>Menstruation</trd>
  </trdgrp>
</drv>
</art>
<!--
$Log$
-->
</vortaro>