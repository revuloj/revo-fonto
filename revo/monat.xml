<drv mrk="monat.tri0o">
  <kap>tri<tld/>o</kap>
  <snc mrk="monat.tri0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      <ref tip="dif" cel="kvar.jar0ono.KAL">Jarkvarono</ref>,
      <ref tip="sin" cel="trimestr.0o">trimestro</ref>:
      <ekz>
        oficistoj proponis cedon de po unu salajra horo &ccirc;iun tri<tld/>on
        <fnt>
          <aut>C. Gacond</aut>,
          <vrk><url ref="http://www.esperanto-gacond.ch/Claude_Gacond_Radioprelego-0019.pdf">19a
          radioprelego</url></vrk>,
          <lok>1963-04-10</lok>
        </fnt>;
      </ekz>
      <ekz>
        plinombrigi la bestkvanton de 600 &gcirc;is 1&#x202F;000 kokidoj dum la unua
        tri<tld/>o 2005
        <fnt>
          <aut>P. Lovesse, trad. D. Durand</aut>,
          <vrk>Bohicon evoluas !</vrk>,
          <lok><url ref="http://www.globidar.org/solidareco/mseo84.htm">monda
          solidareco, informilo 2005-03</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_v;&c_a;&c_r;&c_t;&c_a;&c_l;</trd>,
    <trd>&c_t;&c_r;&c_y;&c_m;&c_e;&c_s;&c_t;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="ca">trimestre</trd>
  <trdgrp lng="de">
    <trd>Trimester</trd>,
    <trd>Quartal</trd>
  </trdgrp>
  <trd lng="fr">trimestre</trd>
  <trdgrp lng="id">
    <trd>kuartal</trd>,
    <trd>trimester</trd>,
    <trd>triwulan</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>trimester</trd>,
    <trd>kwartaal</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kwarta&lstroke;</trd>,
    <trd>trymestr</trd>
  </trdgrp>
  <trd lng="ro">trimestru</trd>
  <trdgrp lng="zh">
