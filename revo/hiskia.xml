<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>hiskiam</rad>/o
</kap>
<drv mrk="hiskia.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="solana.0oj">solanacoj</ref>
      (<trd lng="la">Hyoscyamus</trd>), venena vegeta&jcirc;o.
    </dif>
  </snc>                     
  <trd lng="be">&c_b;&c_l;&c_jo;&c_k;&c_a;&c_t;</trd>
  <trdgrp lng="br">                      
   <trd>louzaouenn-ar-c'housked</trd>,
   <trd>mallc'heot</trd>
  </trdgrp>
  <trd lng="de">Bilsenkraut</trd>
  <trd lng="el">&ypsilon;&omikron;&sigma;&kappa;&ypsilon_ton;&alfa;&my;&omikron;&sigma_fina;</trd>
  <trd lng="fr">jusquiame</trd>
  <trd lng="hu">bel&eacute;ndek</trd>
  <trd lng="nl">bilzenkruid</trd>
  <trd lng="pl">lulek</trd>
  <trd lng="pt">meimendo</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_je;&c_n;&c_a;</trd>
</drv>
</art>
