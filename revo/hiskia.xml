      (<trd lng="la">Hyoscyamus</trd>), venena vegeta&jcirc;o.
    </dif>
  </snc>
  <trd lng="de">Bilsenkraut</trd>
  <trd lng="nl">bilzenkruid</trd>
  <trd lng="pl">lulek</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_je;&c_n;&c_a;</trd>
</drv>
</art>
