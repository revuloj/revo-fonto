<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sxim.xml,v 1.25 2021/05/09 20:48:04 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;im</rad>/i
</kap>
<drv mrk="sxim.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Kovri&gcirc;i per <ref cel="sxim.0o.kovrajxo"><tld/>o</ref>,
      <ref tip="sin" cel="putr.0i">putri</ref>:
      <ekz>
        fari&gcirc;inte posedanto de &ccirc;i tiuj duone <tld/>intaj paperoj, Arkadio, trankvili&gcirc;is
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/2/27/DecayingPeachSmall.gif/200px-DecayingPeachSmall.gif"
      prm="CC BY-SA 2.0"
      >persiko <tld/>anta dum ses tagoj
        <fnt>
          <aut>Andrew Dunn, http://www.andrewdunnphoto.com/, propra verko</aut>
          <lok><url ref="&WCW;/File:DecayingPeachSmall.gif"
              >https://commons.wikimedia.org/wiki/File:DecayingPeachSmall.gif</url></lok>
        </fnt>
    </bld>    
  </snc>
<!-- mankas cita&jcirc;o
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Resti longe forlasita kaj senutila:
      <ekz>
        li <tld/>is en senaktiveco kaj enuo.
      </ekz>
    </dif>
  </snc>
--> 
  <trdgrp lng="be">
    <trd>&c_b;&c_r;&c_o;&c_s;&c_mol;&c_n;&c_je;&c_c;&c_mol;</trd>, 
    <trd>&c_p;&c_l;&c_je;&c_s;&c_mol;&c_n;&c_je;&c_c;&c_mol;</trd>, 
    <trd>&c_c;&c_mol;&c_v;&c_ib;&c_s;&c_mol;&c_c;&c_ib;</trd>
  </trdgrp>
  <trd lng="de">schimmeln</trd>
  <trd lng="el">
&kappa;&alfa;&lambda;&omikron;&ypsilon;&pi;&jota;&alfa_ton;&zeta;&omega;</trd>
  <trdgrp lng="en">
     <trd>mould<klr> (brite)</klr></trd>,
     <trd>mold<klr> (usone)</klr></trd>
  </trdgrp>
  <trd lng="es">enmohecerse</trd>
  <trd lng="fa">&f_k;&f_p;&f_k; &a_z;&a_d;&a_n;</trd>
  <trd lng="fr">moisir</trd>
  <trdgrp lng="hu">
    <trd>pen&eacute;szedik</trd>,
    <trd>poshad</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>ammuffire</trd>,
    <trd>far la <ind>muffa</ind></trd>
  </trdgrp>
  <trd lng="nl">schimmelen</trd>
  <trdgrp lng="pt">
    <trd>mofar</trd>,
    <trd>embolorar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_je;&c_s;&c_n;&c_je;&c_v;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_l;&c_je;&c_s;&c_n;&c_je;&c_v;&c_je;&c_t;&c_mol;
     </trd>
  </trdgrp>
  <trd lng="sv">m&ouml;gla</trd>
</drv>

<drv mrk="sxim.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Putrinta pro <tld/>o:
      <ekz>
        la aran&gcirc;o de la man&gcirc;a&jcirc;oj estis bonega! <tld/>a pano, lardha&ubreve;to, sebaj kandeloj
        kaj kolbaso
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni venas en malaltan arka&jcirc;an kelon <klr>[kie]</klr> &ccirc;io estis malagrabla, kruda
        kaj <tld/>a
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        ilia tuta pano estis malfre&scirc;a kaj <tld/>i&gcirc;inta
        <fnt>
          <bib>MT</bib>
          <lok>Josuo 9:5</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="mucid.0a">mucida</ref>
    </refgrp>
  </snc>
  <trd lng="de">schimmlig</trd>
</drv>

<drv mrk="sxim.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxim.0o.kovrajxo">
    <dif>
      Kovra&jcirc;o de <ref tip="prt" cel="sxim.0fungo"><tld/>fungo</ref>:
      <ekz>
        skeletoj de rosmaroj <klr>[&#8230;]</klr> kovritaj de verdeta <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        longe ku&scirc;inta greno, <klr>[&#8230;]</klr> kiun homoj jam ne volas man&gcirc;i, &ccirc;ar pro
        malnoveco &gcirc;i odoras <tld/>on
        <fnt>
          <bib>QuV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Schimmel <klr tip="amb">(-pilzbelag)</klr></trd>
  </snc>
  <snc mrk="sxim.0o.fungo">
    <dif>
      <ref tip="dif" cel="sxim.0fungo"><tld lit="&Scirc;"/>fungo</ref>:
      <ekz>
        la plej disvasti&gcirc;intaj alergenoj inkluzivas <klr>[&#8230;]</klr> iujn tipojn de poleno
        kaj <tld/>o
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="https://tekstaro.com/t?nomo=artikoloj-el-monato-2012-2018"
              >Alergio &dash; Zorga gardisto nenion preteratentas</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">moho</trd>
  <trd lng="fa">&f_k;&f_p;&f_k;</trd>
  <trd lng="fr">moisissure</trd>
  <trd lng="it">muffa</trd>
  <trd lng="sv">m&ouml;gel</trd>
</drv>

<drv mrk="sxim.0fungo">
  <kap><tld/>fungo</kap>
  <snc>
    <dif>
      Malgranda filamenta 
      <ref tip="lst" cel="fung.0o" lst="voko:fungoj">fungo</ref>
       el la grupo de 
      <ref cel="fung.sak0oj">sakfungoj</ref> 
      (<trd lng="la">Ascomycota</trd>)
      a&ubreve; <trd lng="la">Zygomycota</trd>,
      kreskanta en kaj sur malseka organika materialo,
      ekz. nutra&jcirc;oj ordinare putriganta ilin
      <klr>(kun escepto de kelkaj froma&gcirc;aj kaj kolbasaj
      &scirc;imfungoj)</klr>.
    </dif>
  </snc>
  <trd lng="de">Schmimmelpilz</trd>
</drv>


</art>
