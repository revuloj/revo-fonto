<kap>
  <ofc>*</ofc>
  <rad>&scirc;im</rad>/i
</kap>
<drv mrk="sxim.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Kovri&gcirc;i per <ref cel="sxim.0o.kovrajxo"><tld/>o</ref>,
      <ref tip="sin" cel="putr.0i">putri</ref>:
      <ekz>
        fari&gcirc;inte posedanto de &ccirc;i tiuj duone <tld/>intaj paperoj, Arkadio, trankvili&gcirc;is
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/27/DecayingPeachSmall.gif/200px-DecayingPeachSmall.gif" prm="CC BY-SA         2.0">persiko <tld/>anta dum ses tagoj
        <fnt>
          <aut>Andrew Dunn, http://www.andrewdunnphoto.com/, propra verko</aut>
          <lok><url ref="&WCW;/File:DecayingPeachSmall.gif">&WCW;/File:DecayingPeachSmall.gif</url></lok>
        </fnt>
    </bld>
  </snc>
<!-- mankas cita&jcirc;o
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Resti longe forlasita kaj senutila:
      <ekz>
        li <tld/>is en senaktiveco kaj enuo.
      </ekz>
    </dif>
  </snc>
-->
  <trdgrp lng="be">
    <trd>&c_b;&c_r;&c_o;&c_s;&c_mol;&c_n;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_l;&c_je;&c_s;&c_mol;&c_n;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_c;&c_mol;&c_v;&c_ib;&c_s;&c_mol;&c_c;&c_ib;</trd>
  </trdgrp>
  <trd lng="de">schimmeln</trd>
  <trd lng="el">
&kappa;&alfa;&lambda;&omikron;&ypsilon;&pi;&jota;&alfa_ton;&zeta;&omega;</trd>
  <trdgrp lng="en">
     <trd>mould<klr> (brite)</klr></trd>,
     <trd>mold<klr> (usone)</klr></trd>
  </trdgrp>
  <trd lng="es">enmohecerse</trd>
  <trd lng="fa">&f_k;&f_p;&f_k; &a_z;&a_d;&a_n;</trd>
  <trd lng="fr">moisir</trd>
  <trdgrp lng="hu">
    <trd>pen&eacute;szedik</trd>,
    <trd>poshad</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>ammuffire</trd>,
    <trd>far la <ind>muffa</ind></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x304B;&#x3073;&#x308B;</trd>,
    <trd>&#x30AB;&#x30D3;&#x304C;&#x751F;&#x3048;&#x308B; 
    <pr>&#x30AB;&#x30D3;&#x304C;&#x306F;&#x3048;&#x308B;</pr></trd>,
    <trd>&#x304F;&#x3059;&#x3076;&#x3063;&#x3066;&#x3044;&#x308B;</trd>,
    <trd>&#x7121;&#x70BA;&#x306B;&#x904E;&#x3054;&#x3059;  <pr>&#x3080;&#x3044;&#x306B;&#x3059;&#x3054;&#x3059;</pr></trd>
  </trdgrp>
  <trd lng="nl">schimmelen</trd>
  <trdgrp lng="pl">
    <trd>ple&sacute;nie&cacute;</trd>,
    <trd>zgni&cacute;</trd>,
    <trd>butwie&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>mofar</trd>,
    <trd>embolorar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_je;&c_s;&c_n;&c_je;&c_v;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_l;&c_je;&c_s;&c_n;&c_je;&c_v;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">m&ouml;gla</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43B;&c_ib;&c_s;&#x43D;&#x44F;&c_v;&c_ib;&c_t;&c_i;</trd>,
    <trd>&#x43F;&#x43E;&#x43A;&c_r;&c_i;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;
&c_c;&c_v;&c_ib;&#x43B;&#x43B;&#x44E;/&#x43F;&#x43B;&c_ib;&c_s;&c_je;&#x43D;&#x43D;&#x44E;/&#x43F;&#x43B;&c_ib;&c_s;&#x43D;&#x44F;&c_v;&#x43E;&#x44E;</trd>
    </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x53D1;&#x9709; <pr>f&#x101;m&#xE9;i</pr></trd>,
    <trd>&#x767C;&#x9709; <pr>f&#x101;m&#xE9;i</pr></trd>,
    <trd>&#x9709;&#x70C2; <pr>m&#xE9;il&#xE0;n</pr></trd>,
    <trd>&#x9709;&#x721B; <pr>m&#xE9;il&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxim.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Putrinta pro <tld/>o:
      <ekz>
        la aran&gcirc;o de la man&gcirc;a&jcirc;oj estis bonega! <tld/>a pano, lardha&ubreve;to, sebaj kandeloj
        kaj kolbaso
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni venas en malaltan arka&jcirc;an kelon <klr>[kie]</klr> &ccirc;io estis malagrabla, kruda
        kaj <tld/>a
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        ilia tuta pano estis malfre&scirc;a kaj <tld/>i&gcirc;inta
        <fnt>
          <bib>MT</bib>
          <lok>Josuo 9:5</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="mucid.0a">mucida</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_mol;&c_v;&c_ib;&c_l;&c_y;</trd>,
    <trd>&c_s;&c_p;&c_l;&c_ja;&c_s;&c_mol;&c_n;&c_je;&c_l;&c_y;</trd>,
    <trd>&c_z;&c_b;&c_r;&c_o;&c_s;&c_mol;&c_n;&c_je;&c_l;&c_y;</trd>
  </trdgrp>
  <trd lng="de">schimmlig</trd>
  <trdgrp lng="ja">
    <trd>&#x30AB;&#x30D3;&#x306E;  <pr>&#x304B;&#x3073;&#x306E;</pr></trd>,
    <trd>&#x304B;&#x3073;&#x305F;</trd>,
    <trd>&#x30AB;&#x30D3;&#x306E;&#x751F;&#x3048;&#x305F; 
    <pr>&#x304B;&#x3073;&#x306E;&#x306F;&#x3048;&#x305F;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>st&eogonek;ch&lstroke;y</trd>,
    <trd>sple&sacute;nia&lstroke;y</trd>,
    <trd>zbutwia&lstroke;y</trd>,
    <trd>zmursza&lstroke;y</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43B;&c_ib;&c_s;&#x43D;&#x44F;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_c;&c_v;&c_ib;&#x43B;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="sxim.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxim.0o.kovrajxo">
    <dif>
      Kovra&jcirc;o de <ref tip="prt" cel="sxim.0fungo"><tld/>fungo</ref>:
      <ekz>
        skeletoj de rosmaroj <klr>[&#x2026;]</klr> kovritaj de verdeta <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        longe ku&scirc;inta greno, <klr>[&#x2026;]</klr> kiun homoj jam ne volas man&gcirc;i, &ccirc;ar pro
        malnoveco &gcirc;i odoras <tld/>on
        <fnt>
          <bib>QuV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Schimmel <klr tip="amb">(-pilzbelag)</klr></trd>
  </snc>
  <snc mrk="sxim.0o.fungo">
    <dif>
      <ref tip="dif" cel="sxim.0fungo"><tld lit="&Scirc;"/>fungo</ref>:
      <ekz>
        la plej disvasti&gcirc;intaj alergenoj inkluzivas <klr>[&#x2026;]</klr> iujn tipojn de poleno
        kaj <tld/>o
        <fnt>
        <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2013/010953.php">Alergio &dash; Zorga
          gardisto nenion preteratentas</url></vrk>
           <bib>Monato</bib>
           <lok>2013/04, p. 23</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_l;&c_je;&c_s;&c_mol;&c_n;&c_ja;</trd>,
    <trd>&c_b;&c_r;&c_o;&c_s;&c_mol;&c_n;&c_ja;</trd>,
    <trd>&c_c;&c_mol;&c_v;&c_ib;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">floridura</trd>
  <trd lng="cs">pl&iacute;se&ncaron;</trd>
  <trd lng="es">moho</trd>
  <trdgrp lng="en">
    <trd>mould</trd>,
    <trd>mold</trd>
  </trdgrp>
  <trd lng="fa">&f_k;&f_p;&f_k;</trd>
  <trd lng="fr">moisissure</trd>
  <trdgrp lng="gl">
    <trd>mofo</trd>,
    <trd>balor</trd>
  </trdgrp>
  <trd lng="hu">pen&eacute;sz</trd>
  <trd lng="it">muffa</trd>
  <trd lng="ja">&#x30AB;&#x30D3;</trd>
  <trd lng="nl">schimmel</trd>
  <trd lng="no">mugg</trd>
  <trd lng="pl">ple&sacute;&nacute;</trd>
  <trdgrp lng="pt">
    <trd>mofo</trd>,
    <trd>bolor</trd>
  </trdgrp>
  <trd lng="sv">m&ouml;gel</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43B;&c_ib;&c_s;&c_je;&#x43D;&#x44C;</trd>,
    <trd>&#x43F;&#x43B;&c_ib;&c_s;&#x43D;&#x44F;&c_v;&c_a;</trd>,
    <trd>&c_c;&c_v;&c_ib;&#x43B;&#x44C;</trd>
  </trdgrp>
  <trd lng="zh">&#x9709;&#x83CC; <pr>m&#xE9;ij&#x16B;n</pr></trd>
</drv>

<drv mrk="sxim.0fungo">
  <kap><tld/>fungo</kap>
  <snc>
    <dif>
      Malgranda filamenta
      <ref tip="lst" cel="fung.0o" lst="voko:fungoj">fungo</ref>
       el la grupo de
      <ref cel="fung.sak0oj">sakfungoj</ref>
      (<trd lng="la">Ascomycota</trd>)
      a&ubreve; <trd lng="la">Zygomycota</trd>,
      kreskanta en kaj sur malseka organika materialo,
      ekz. nutra&jcirc;oj ordinare putriganta ilin
      <klr>(kun escepto de kelkaj froma&gcirc;aj kaj kolbasaj
      &scirc;imfungoj)</klr>.
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_mol;&c_v;&c_ib;&c_l;&c_jo;&c_v;&c_y; <ind>&c_g;&c_r;&c_y;&c_b;</ind></trd>
  <trd lng="de">Schmimmelpilz</trd>
  <trd lng="pl">grzyb <ind>ple&sacute;niowy</ind></trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43B;&c_ib;&c_s;&c_je;&#x43D;&#x44C;</trd>,
    <trd>&#x43F;&#x43B;&c_ib;&c_s;&#x43D;&#x44F;&c_v;&c_a;</trd>,
    <trd>&c_c;&c_v;&c_ib;&#x43B;&#x44C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9709;&#x83CC; <pr>m&#xE9;ij&#x16B;n</pr></trd>,
    <trd>&#x9EF4;&#x83CC; <pr>m&#xE9;ij&#xF9;n</pr></trd>
  </trdgrp>
</drv>
</art>
