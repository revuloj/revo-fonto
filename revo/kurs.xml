<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>1</ofc>
<rad>kurs</rad>/o</kap>
<drv mrk="kurs.0o">
  <kap><tld/>o</kap>
  <snc mrk="kurs.0o.lerna">
    <uzo tip="fak">PED</uzo>
    <dif>
      Serio da <ref tip="prt" cel="lecion.0o">lecionoj</ref>,
      konsistiganta regulan instruon pri scienco, arto a&ubreve; metio:
      <ekz>
        <tld/>oj pri la bretona lingvo
        <fnt><aut>Josette Ducloyer</aut>: <vrk>Bretonio</vrk>,
        <bib>Monato</bib><lok>jaro 2000a, numero 2a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_u;&c_r;&c_s; <klr>(&c_n;&c_a;&c_v;&c_u;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_y;)</klr></trd>
    <trd lng="ca">curs</trd>
    <trd lng="en">course</trd>
    <trd lng="es">curso</trd>
    <trdgrp lng="fr">
      <trd>cours<klr> (enseignement)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>tanfolyam</trd>,
      <trd>kurzus</trd>
    </trdgrp>
    <trd lng="nl">cursus</trd>
    <trd lng="ru">&c_k;&c_u;&c_r;&c_s;</trd>
  </snc>
  <snc>
    <uzo tip="stl">EVI</uzo>
    <ref tip="dif" cel="kurz.0o">kurzo</ref>
  </snc>
  <snc mrk="kurs.0o.voja">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Vojlinio la&ubreve; kiu io movi&gcirc;as a&ubreve; voja&gcirc;as:
      <ekz>
        la po&scirc;ta&jcirc;oj estas specigitaj la&ubreve; la germanaj <tld/>oj
        ... la po&scirc;tistoj devas scii, al kiu <tld/>o &gcirc;i
        <klr>(la loko indikita en letera adreso)</klr>
        apartenas<fnt><aut>&Z;</aut>: <vrk>Fundamenta Krestomatio</vrk>,
        <lok>El la po&scirc;to</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>o de a&ubreve;tobuso<fnt>PIV</fnt>,
      </ekz>
      <ekz>
        la &scirc;ipo tenis sian <tld/>on norden<fnt>PIV</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_u;&c_r;&c_s;</trd>,
      <trd>&c_m;&c_a;&c_r;&c_sh;&c_r;&c_u;&c_t;</trd>
    </trdgrp>
    <trd lng="en">course</trd>
    <trdgrp lng="fr">
      <trd>cours<klr> (trajet)</klr></trd>,
      <trd>route<klr> (trajet)</klr></trd>,
      <trd>trajet</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&uacute;tvonal</trd>,
      <trd>&uacute;tir&aacute;ny</trd>
    </trdgrp>
    <trd lng="nl">koers<klr tip="ind"> (v. boot bijv.)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_u;&c_r;&c_s;</trd>,
      <trd>&c_m;&c_a;&c_r;&c_sh;&c_r;&c_u;&c_t;</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">kurz (&scaron;kolen&iacute;)</trd>
  <trd lng="de">Kurs</trd>
  <trd lng="sv">kurs</trd>
</drv>

<drv mrk="kurs.0ano">
  <kap><tld/>ano</kap>
  <uzo tip="fak">PED</uzo>
  <snc>
    <dif>
      Lernanto de <ref tip="vid" cel="kurs.0o.lerna">kurso</ref>:
      <ekz>
        <tld/>anoj devas atente legi
        <fnt><aut>Andr&eacute; Staes</aut>: <vrk>A&ccirc;etu grandskale</vrk>,
        <bib>Monato</bib><lok>jaro 1994a, numero 1a, p. 23a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_l;&c_u;&c_h;&c_a;&c_ch; &c_k;&c_u;&c_r;&c_s;&c_u;</trd>,
    <trd>&c_k;&c_u;&c_r;&c_s;&c_a;&c_n;&c_t;</trd>
  </trdgrp>
  <trd lng="de">Kursteilnehmer</trd>
  <trd lng="es">cursillista</trd>
  <trdgrp lng="fr">
    <trd>&eacute;l&egrave;ve<klr> (d'un cours)</klr></trd>
  </trdgrp>
  <trd lng="hu">tanfolyamhallgat&oacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_u;&c_sh;&c_a;&c_t;&c_je;&c_l;&c_mol; &c_k;&c_u;&c_r;&c_s;&c_a;</trd>,
    <trd>&c_k;&c_u;&c_r;&c_s;&c_a;&c_n;&c_t;</trd>
  </trdgrp>
  <trd lng="nl">cursist</trd>
</drv>

<drv mrk="kurs.en0igi">
  <kap>en<tld/>igi</kap> <fnt>PIV</fnt>
  <uzo tip="fak">POSX</uzo>
  <snc>
    <dif>
      Zorgi ke leteroj iru la&ubreve; sia
      <ref tip="vid" cel="kurs.0o.voja">kurso</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>sortieren</trd>,
    <trd>verteilen</trd>
  </trdgrp>
  <trd lng="es">encaminar</trd>
  <trdgrp lng="fr">
    <trd>router</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&eacute;tv&aacute;logat <klr tip="ind">(post&aacute;n)</klr></trd>,
    <trd>szort&iacute;roz <klr tip="ind">(post&aacute;n)</klr></trd>
  </trdgrp>
  <trd lng="nl">in <ind>omloop</ind> brengen</trd>
</drv>

<drv mrk="kurs.en0igilo">
  <kap>en<tld/>igilo, <tld/>ilo</kap>
  <fnt><lok><url
  ref="&KompLeks;/E.html#ENKURSIGILO">KompLeks</url></lok></fnt>
  <snc>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      En komputila reto, aran&gcirc;a&jcirc;o ricevanta mesa&gcirc;ojn (paka&jcirc;ojn), kaj
      plusendanta ilin al la celata komputilo per preferinda vojo
      (<ref cel="kurs.0o.voja">kurso</ref>).
    </dif>
    <ref tip="vid" cel="kluz.0o">kluzo</ref>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_sh;&c_r;&c_u;&c_t;&c_y;&c_z;&c_a;&c_t;&c_a;&c_r;</trd>
  <trd lng="en">router</trd>
  <trd lng="es">encaminador</trd>
  <trd lng="fr">routeur</trd>
  <trdgrp lng="hu">
    <trd>router</trd>,
    <trd>&uacute;tvonalv&aacute;laszt&oacute;</trd>,
    <trd>adat&uacute;tv&aacute;laszt&oacute;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ruter</trd>,
    <trd>przeka&zacute;nik</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>router</trd>,
    <trd>verkeerswisselaar<klr tip="ind"> (comp.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_a;&c_r;&c_sh;&c_r;&c_u;&c_t;&c_i;&c_z;&c_a;&c_t;&c_o;&c_r;</trd>
</drv>
</art>
