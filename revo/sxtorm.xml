<drv mrk="sxtorm.0o">
  <kap><ofc>9</ofc><tld/>o  <fnt><aut>M. Rollet de l'Isle</aut>,
  <vrk>Provo de marista terminaro</vrk>, <lok>1908</lok></fnt></kap>
  <snc mrk="sxtorm.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Forta, dan&gcirc;era
      <ref tip="vid" cel="vent.0ego.MET">ventego</ref>:
      <ekz>
        la fulmotondro i&gcirc;is uragana <tld/>o
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        dikegaj uloj kun kurbaj kaj nodoplenaj bran&ccirc;egoj kiuj dum jarcentoj kaj jarmilo
        eltenis <tld/>ojn
        <fnt>
          <bib>ViV</bib>
        </fnt>;
      </ekz>
      <ekz>
        sova&gcirc;a <tld/>o pelis la pezajn nubojn kiel paperpecojn sur la nokta &ccirc;ielo
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="tondr.fulmo0o">fulmotondro</ref>,
       <ref cel="tempes.0o">tempesto</ref>,
       <ref cel="uragan.0o.MET">uragano<sncref/></ref>
    </refgrp>
  </snc>
  <snc mrk="sxtorm.0o.atako">
    <dif>
      Rapida kaj forta <ref tip="super" cel="atak.0o">atako</ref>:
      <ekz>
        <tld/>o de la indigno kaptas subite la malgrandan salonon
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de apla&ubreve;dado, kvaza&ubreve; bruego de tertremo, ekskuis la placon
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>oj de la vivo
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        Esperanto rezistis la <tld/>ojn kaj sangan elprovon sub fa&scirc;ismo
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        la psika <tld/>o subite paci&gcirc;is, kiam &scirc;i levis la rigardon <klr>[&#8230;]</klr> kaj
        amba&ubreve; samsekunde ridetis
        <fnt>
          <bib>ChM</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_t;&c_o;&c_r;&c_m;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&t_ra;&t_lata;&t_shabchu;&t_nga;&t_tsheg;&t_-a;&t_tsha;&t_shabchu;&t_ba;&t_tsheg;</trd>
  <trd lng="de">Sturm</trd>
  <trd lng="el">&theta;&ypsilon_ton;&epsilon;&lambda;&lambda;&alfa;
  </trd>
  <trd lng="en">storm</trd>
  <trdgrp lng="es">
    <trd>tormenta</trd>
    <trd>tempestad</trd>
  </trdgrp>
  <trd lng="fr">temp&ecirc;te</trd>
  <trd lng="hu">sz&eacute;lvihar</trd>
  <trdgrp lng="it">
    <trd>temporale<klr> (di vento)</klr></trd>,
    <trd>tempesta<klr> (di vento, di sabbia, ecc...)
  </klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#26292;&#39080;  <pr>&#12412;&#12358;&#12405;&#12358;</pr></trd>,
    <trd>&#12354;&#12425;&#12375;</trd>,
    <trd>&#24375;&#39080;  <pr>&#12365;&#12423;&#12358;&#12405;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">storm</trd>
  <trdgrp lng="pl">
    <trd>burza</trd>,
    <trd>sztorm</trd>,
    <trd>wichura</trd>
  </trdgrp>
  <trd lng="pt">tempestade</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_t;&c_o;&c_r;&c_m;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_ja;</trd>
  </trdgrp>
  <trd lng="sv">storm</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_r;&c_ja;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_a;&c_n;</trd>,
    <trd>&c_sh;&c_t;&c_o;&c_r;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#39080;&#26292; <pr>f&emacron;ngb&agrave;o</pr></trd>,
    <trd>&#39118;&#26292; <pr>f&emacron;ngb&agrave;o</pr></trd>,
    <trd>&#26292;&#39080; <pr>b&agrave;of&emacron;ng</pr></trd>,
    <trd>&#26292;&#39118; <pr>b&agrave;of&emacron;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxtorm.0a">
  <kap>
    <tld/>a
  </kap>
  <snc mrk="sxtorm.0a.MET">
    <dif>
      Trafata, agitata de <tld/>o:
      <ekz>
        post longa na&gcirc;ado en <tld/>a oceano, jen li trafis florplenan insulon
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la &scirc;ipo]</klr> devis batali kontra&ubreve; <tld/>a vetero
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[li]</klr> veki&gcirc;is el <tld/>a son&gcirc;o
        <fnt>
          <bib>MMa</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_u;&c_r;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_b;&c_u;&c_sh;&c_a;&c_w;&c_n;&c_y;</trd>
