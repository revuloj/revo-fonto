    <trd>&#33618;&#22825;&#x306E;  <pr>&#12371;&#12358;&#12390;&#12435;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">stormachtig</trd>
  <trdgrp lng="pl">
    <trd>burzliwy</trd>,
    <trd>nawalny</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_r;&#x44F;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_je;&#x43C;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_u;&c_r;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_h;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_sh;&c_t;&#x43E;&c_r;&#x43C;&#x43E;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4E0D;&#x7A33;&#x5B9A;&#x6C14;&#x6D41;
    <pr>b&#xF9;w&#x11B;nd&#xEC;ngq&#xEC;li&#xFA;</pr></trd>,
    <trd>&#x4E0D;&#x7A69;&#x5B9A;&#x6C23;&#x6D41;
    <pr>b&#xF9;w&#x11B;nd&#xEC;ngq&#xEC;li&#xFA;</pr></trd>,
    <trd>&#x6CBA; <pr>ti&#xE1;n</pr></trd>,
    <trd>&#x8D77;&#22823;&#x98A8; <pr>q&#x1D0;d&#xE0;if&emacron;ng</pr></trd>,
    <trd>&#x8D77;&#22823;&#x98CE; <pr>q&#x1D0;d&#xE0;if&emacron;ng</pr></trd>,
    <trd>&#x591A;&#x98A8;&#x66B4; <pr>du&#x14D;f&emacron;ngb&#xE0;o</pr></trd>,
    <trd>&#x591A;&#x98CE;&#x66B4; <pr>du&#x14D;f&emacron;ngb&#xE0;o</pr></trd>,
    <trd>&#22914;&#x706B;&#22914;&#33590; <pr>r&#xFA;hu&#x1D2;r&#xFA;ch&#xE1;</pr></trd>,
    <trd>&#26377;&#x66B4;&#x98A8;&#x96E8; <pr>y&#x1D2;ub&#xE0;of&emacron;ngy&#x1D4;</pr></trd>,
    <trd>&#26377;&#x66B4;&#x98CE;&#x96E8; <pr>y&#x1D2;ub&#xE0;of&emacron;ngy&#x1D4;</pr></trd>,
    <trd>&#x72C2;&#x98A8;&#x66B4;&#x96E8; <pr>ku&#xE1;ngf&emacron;ngb&#xE0;oy&#x1D4;</pr></trd>,
    <trd>&#x72C2;&#x98CE;&#x66B4;&#x96E8; <pr>ku&#xE1;ngf&emacron;ngb&#xE0;oy&#x1D4;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxtorm.0o">
  <kap><ofc>9</ofc><tld/>o  <fnt><aut>M. Rollet de l'Isle</aut>,
  <vrk>Provo de marista terminaro</vrk>, <lok>1908</lok></fnt></kap>
  <snc mrk="sxtorm.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Forta, dan&gcirc;era
      <ref tip="vid" cel="vent.0ego.MET">ventego</ref>:
      <ekz>
        la fulmotondro i&gcirc;is uragana <tld/>o
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        dikegaj uloj kun kurbaj kaj nodoplenaj bran&ccirc;egoj kiuj dum jarcentoj kaj jarmilo
        eltenis <tld/>ojn
        <fnt>
          <bib>ViV</bib>
        </fnt>;
      </ekz>
      <ekz>
        sova&gcirc;a <tld/>o pelis la pezajn nubojn kiel paperpecojn sur la nokta &ccirc;ielo
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="tondr.fulmo0o">fulmotondro</ref>,
       <ref cel="tempes.0o">tempesto</ref>,
       <ref cel="uragan.0o.MET">uragano<sncref/></ref>
    </refgrp>
  </snc>
  <snc mrk="sxtorm.0o.atako">
    <dif>
      Rapida kaj forta <ref tip="super" cel="atak.0o">atako</ref>:
      <ekz>
        <tld/>o de la indigno kaptas subite la malgrandan salonon
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de apla&ubreve;dado, kvaza&ubreve; bruego de tertremo, ekskuis la placon
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>oj de la vivo
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        Esperanto rezistis la <tld/>ojn kaj sangan elprovon sub fa&scirc;ismo
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        la psika <tld/>o subite paci&gcirc;is, kiam &scirc;i levis la rigardon <klr>[&#8230;]</klr> kaj
        amba&ubreve; samsekunde ridetis
        <fnt>
          <bib>ChM</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_t;&c_o;&c_r;&c_m;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&t_ra;&t_lata;&t_shabchu;&t_nga;&t_tsheg;&t_-a;&t_tsha;&t_shabchu;&t_ba;&t_tsheg;</trd>
  <trd lng="de">Sturm</trd>
  <trd lng="el">&theta;&ypsilon_ton;&epsilon;&lambda;&lambda;&alfa;
  </trd>
  <trd lng="en">storm</trd>
  <trdgrp lng="es">
    <trd>tormenta</trd>
    <trd>tempestad</trd>
  </trdgrp>
  <trd lng="fr">temp&ecirc;te</trd>
  <trd lng="hu">sz&eacute;lvihar</trd>
  <trdgrp lng="it">
    <trd>temporale<klr> (di vento)</klr></trd>,
    <trd>tempesta<klr> (di vento, di sabbia, ecc...)
  </klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x66B4;&#x98A8;  <pr>&#x307C;&#12358;&#12405;&#12358;</pr></trd>,
    <trd>&#12354;&#12425;&#12375;</trd>,
    <trd>&#x5F37;&#x98A8;  <pr>&#x304D;&#12423;&#12358;&#12405;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">storm</trd>
  <trdgrp lng="pl">
    <trd>burza</trd>,
    <trd>sztorm</trd>,
    <trd>wichura</trd>
  </trdgrp>
  <trd lng="pt">tempestade</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_t;&c_o;&c_r;&c_m;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_ja;</trd>
  </trdgrp>
  <trd lng="sv">storm</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_r;&#x44F;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_a;&#x43D;</trd>,
    <trd>&c_sh;&c_t;&#x43E;&c_r;&#x43C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x98A8;&#x66B4; <pr>f&emacron;ngb&#xE0;o</pr></trd>,
    <trd>&#x98CE;&#x66B4; <pr>f&emacron;ngb&#xE0;o</pr></trd>,
    <trd>&#x66B4;&#x98A8; <pr>b&#xE0;of&emacron;ng</pr></trd>,
    <trd>&#x66B4;&#x98CE; <pr>b&#xE0;of&emacron;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxtorm.kirlo0o">
  <kap>kirlo<tld/>o</kap>
  <snc mrk="sxtorm.kirlo0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      <ref tip="super" cel="sxtorm.0o.MET">&Scirc;tormo<sncref/></ref>
      kiu vortice turni&gcirc;as:
      <ekz>
        dum neniu anta&ubreve;a jaro <ind>kirlo<tld/>o</ind>j estis tiel fortaj, &gcirc;is 280 km/h rapidegaj
        <fnt>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2005/008822.php">Du mil kvin</url></vrk>
          <bib>Monato</bib>
          <lok>2005/12, p. 7</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="ciklon.0o.MET">ciklono</ref>
  </refgrp>
  <trd lng="be">&c_c;&c_y;&c_k;&c_l;&c_jo;&c_n;</trd>
  <trd lng="de">Wirbelsturm</trd>
  <trd lng="fr">tornade</trd>
  <trdgrp lng="nl">
    <trd>wervelstorm</trd>,
    <trd>tornado</trd>
  </trdgrp>
  <trd lng="pl">cyklon</trd>
  <trd lng="uk">&c_c;&c_i;&#x43A;&#x43B;&#x43E;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x9F8D;&#25458;&#x98A8; <pr>l&#xF3;ngju&#x1CE;nf&emacron;ng</pr></trd>,
    <trd>&#x9F99;&#21367;&#x98CE; <pr>l&#xF3;ngju&#x1CE;nf&emacron;ng</pr></trd>,
    <trd>&#21367;&#x626C; <pr>ju&#x1CE;ny&#xE1;ng</pr></trd>,
    <trd>&#25458;&#x63DA; <pr>ju&#x1CE;ny&#xE1;ng</pr></trd>,
    <trd>&#x65CB;&#x98A8; <pr>xu&#xE0;nf&emacron;ng</pr></trd>,
    <trd>&#x65CB;&#x98CE; <pr>xu&#xE0;nf&emacron;ng</pr></trd>,
    <trd>&#x66B4;&#x98A8;&#x9A5F;&#x96E8;&#x98B6;
    <pr>b&#xE0;of&emacron;ngzh&#xF2;uy&#x1D4;j&#xF9;</pr></trd>,
    <trd>&#x66B4;&#x98CE;&#x9AA4;&#x96E8;&#x98D3;
    <pr>b&#xE0;of&emacron;ngzh&#xF2;uy&#x1D4;j&#xF9;</pr></trd>,
