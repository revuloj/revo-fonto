<drv mrk="sxtorm.0i">
  <kap><tld/>i</kap>
  <snc mrk="sxtorm.0i.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Forte, dan&gcirc;ere <ref tip="super" cel="vent.0i">venti</ref>,
      eventuale kun pluvo, ne&gcirc;o, sablo...:
      <ekz>
        se super ili <tld/>us la ventego, &ccirc;i tie restus same kviete
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;<tld/>os,&rightquot; <nom>Donald</nom> gruntis moroze
        <fnt>
          <aut>J. Francis</aut>
          <vrk>La Granda Kaldrono</vrk>
          <lok>1978</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la batalo jam <tld/>is viglege
        <fnt>
          <bib>ChV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_u;&c_sh;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_z;&c_b;&c_u;&c_r;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_sh;&c_t;&c_a;&c_r;&c_m;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">st&uuml;rmen</trd>
  <trdgrp lng="es">
    <trd>tempestear</trd>,
    <trd>haber tormenta</trd>
  </trdgrp>
  <trd lng="fr">temp&ecirc;ter</trd>
  <trdgrp lng="ja">
    <trd>&#26292;&#39080;&#12364;&#21561;&#12367;  <pr>&#12412;&#12358;&#12405;&#12358;&#12364;&#12405;&#12367;</pr></trd>,
    <trd>&#33618;&#12428;&#12427;  <pr>&#12354;&#12428;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">stormen</trd>
  <trdgrp lng="pl">
    <trd>bardzo silnie <ind>wia&cacute;</ind></trd>
    <trd>dosta&cacute; furii</trd>,
    <trd>wpa&sacute;&cacute; we w&sacute;ciek&lstroke;o&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_sh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_sh;&c_a;&c_l;&c_je;&c_n;&c_ib;&c_t;&c_i; (&c_p;&c_r;&c_o; &c_b;&c_u;&c_r;&c_ju;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#39080;&#26292;&#29378;&#39080; <pr>f&emacron;ngb&agrave;oku&aacute;ngf&emacron;ng</pr></trd>,
    <trd>&#39118;&#26292;&#29378;&#39118; <pr>f&emacron;ngb&agrave;oku&aacute;ngf&emacron;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxtorm.kirlo0o">
  <kap>kirlo<tld/>o</kap>
  <snc mrk="sxtorm.kirlo0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      <ref tip="super" cel="sxtorm.0o.MET">&Scirc;tormo<sncref/></ref>
      kiu vortice turni&gcirc;as:
      <ekz>
        dum neniu anta&ubreve;a jaro <ind>kirlo<tld/>o</ind>j estis tiel fortaj, &gcirc;is 280 km/h rapidegaj
        <fnt>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2005/008822.php">Du mil kvin</url></vrk>
          <bib>Monato</bib>
          <lok>2005/12, p. 7</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ciklon.0o.MET">ciklono</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_k;&c_l;&c_jo;&c_n;</trd>
  <trd lng="de">Wirbelsturm</trd>
  <trd lng="fr">tornade</trd>
