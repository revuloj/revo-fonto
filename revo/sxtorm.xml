<drv mrk="sxtorm.sabl0o">
  <kap>sablo<tld/>o</kap>
  <snc mrk="sxtorm.sabl0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      <ref tip="super" cel="sxtorm.0o.MET">&Scirc;tormo<sncref/></ref>,
      ventego kiu kunportas sablon kaj polvon:
      <ekz>
        de pratempoj la sablo<tld/>o &scirc;lif<klr>[i]</klr>s iliajn e&gcirc;ojn kaj malglata&jcirc;ojn
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Goabo, la dezerto de la Koloroj</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni povus &gcirc;uste imagi la bezonon de vualo kiel masko kontra&ubreve; sablo-<tld/>oj kaj
        brula varmo sub la dezerta suno
        <fnt>
          <aut>H. Al-Amily</aut>
          <vrk><url ref="&Monato;/2007/009296.php">La vualo, romantika kaj erotika</url></vrk>
          <bib>Monato</bib>
          <lok>2007/11, p. 21</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="tromb.0o.sablo">trombo<sncref/></ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ja;&c_s;&c_ch;&c_a;&c_n;&c_a;&c_ja; <ind>&c_b;&c_u;&c_r;&c_a;</ind></trd>,
    <trd>&c_p;&c_y;&c_l;&c_a;&c_v;&c_a;&c_ja; <ind>&c_b;&c_u;&c_r;&c_a;</ind></trd>,
    <trd>&c_p;&c_y;&c_l;&c_a;&c_v;&c_a;&c_ja; <ind>&c_z;&c_a;&c_v;&c_ib;&c_r;&c_u;&c_h;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="de">Sandsturm</trd>
  <trd lng="es">tormenta<klr> (de arena)</klr></trd>
  <trd lng="fr">temp&ecirc;te de sable</trd>
  <trd lng="it"><ind>tempesta</ind> di sabbia</trd>
  <trd lng="ja">&#30722;&#23888;  <pr>&#12377;&#12394;&#12354;&#12425;&#12375;</pr></trd>
  <trd lng="nl">zandstorm</trd>
  <trd lng="pl">burza piaskowa</trd>
  <trd lng="uk">&c_p;&c_ib;&c_shch;&c_a;&c_n;&c_a; &c_b;&c_u;&c_r;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#27801;&#22645;&#26292; <pr>sh&amacron;ch&eacute;nb&agrave;o</pr></trd>,
    <trd>&#27801;&#23576;&#26292; <pr>sh&amacron;ch&eacute;nb&agrave;o</pr></trd>,
    <trd>&#28784;&#38718; <pr>hu&imacron;m&aacute;i</pr></trd>,
    <trd>&#27801;&#26292; <pr>sh&amacron;b&agrave;o</pr></trd>,
