<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&scirc;torm</rad>/o
</kap>
<drv mrk="sxtorm.0o">
  <kap><tld/>o<fnt>R</fnt></kap>
  <snc mrk="sxtorm.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Forta, dan&gcirc;era <ref tip="vid" cel="vent.0ego.MET">ventego</ref>:
      <ekz>
        amo estas <tld/>a maro senhavena kaj senborda;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu decido elvokis <tld/>on de indigno.
      </ekz>
    </dif>
    <ref tip="sin" cel="tempes.0o">tempesto</ref>
  </snc>
  <trd lng="be">&c_sh;&c_t;&c_o;&c_r;&c_m;</trd>
  <trd lng="de">Sturm</trd>
  <trd lng="el">&theta;&ypsilon_ton;&epsilon;&lambda;&lambda;&alfa;</trd>
  <trd lng="en">storm</trd>
  <trd lng="fr">temp&ecirc;te</trd>
  <trd lng="hu">sz&eacute;lvihar</trd>
  <trd lng="nl">storm</trd>
  <trd lng="pt">tempestade</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_t;&c_o;&c_r;&c_m;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_ja;</trd>
  </trdgrp>
  <trd lng="sv">storm</trd>
</drv>
</art>
