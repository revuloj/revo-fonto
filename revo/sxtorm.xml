<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxtorm.xml,v 1.26 2021/05/29 14:33:19 revo Exp $">
<kap>
  <ofc>9</ofc><rad>&scirc;torm</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sxtorm.0i">
  <kap><tld/>i</kap>
  <snc mrk="sxtorm.0i.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Forte, dan&gcirc;ere venti, eventuale kun pluvo, ne&gcirc;o, sablo...:
      <ekz>
        &leftquot;<tld/>os,&rightquot; Donald gruntis moroze
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd>tempestear</trd>,
    <trd>haber tormenta</trd>
  </trdgrp>
  <trd lng="fr">temp&ecirc;ter</trd>
</drv>

<drv mrk="sxtorm.0o">
  <kap><ofc>9</ofc><tld/>o  <fnt><aut>M. Rollet de l'Isle</aut>,
  <vrk>Provo de marista terminaro</vrk>, <lok>1908</lok></fnt></kap>
  <snc mrk="sxtorm.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Forta, dan&gcirc;era
      <ref tip="vid" cel="vent.0ego.MET">ventego</ref>:
      <ekz>
        amo estas <tld/>a maro senhavena kaj senborda;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu decido elvokis <tld/>on de indigno.
      </ekz>
    </dif>
    <ref tip="vid" cel="tempes.0o">tempesto</ref>
  </snc>
  <trd lng="be">&c_sh;&c_t;&c_o;&c_r;&c_m;</trd>
  <trd lng="bo">&#xF62;&#xFB3;&#xF74;&#xF44;&#xF0B;&#xF60;&#xF5A;&#xF74;&#xF56;&#xF0B;</trd>
  <trd lng="de">Sturm</trd>
  <trd lng="el">&theta;&ypsilon_ton;&epsilon;&lambda;&lambda;&alfa;
  </trd>
  <trd lng="en">storm</trd>
  <trdgrp lng="es">
    <trd>tormenta</trd>
    <trd>tempestad</trd>
  </trdgrp>
  <trd lng="fr">temp&ecirc;te</trd>
  <trd lng="hu">sz&eacute;lvihar</trd>
  <trdgrp lng="it">
    <trd>temporale<klr> (di vento)</klr></trd>,
    <trd>tempesta<klr> (di vento, di sabbia, ecc...)
  </klr></trd>
  </trdgrp>
  <trd lng="nl">storm</trd>
  <trd lng="pt">tempestade</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_t;&c_o;&c_r;&c_m;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_ja;</trd>
  </trdgrp>
  <trd lng="sv">storm</trd>
</drv>

<drv mrk="sxtorm.sabl0o">
  <kap>sablo<tld/>o</kap>
  <snc mrk="sxtorm.sabl0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      &Scirc;tormo a&ubreve; ventego kiu kunportas sablon kaj polvon.
    </dif>
  </snc>
  <trd lng="de">Sandsturm</trd>
  <trd lng="es">tormenta<klr> (de arena)</klr></trd> 
  <trd lng="fr">temp&ecirc;te<klr> (de sable)</klr></trd> 
  <trd lng="it"><ind>tempesta</ind> di sabbia</trd>
</drv>
</art>
