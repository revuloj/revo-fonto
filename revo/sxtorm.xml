<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxtorm.xml,v 1.33 2022/05/05 14:43:49 revo Exp $">
<kap>
  <ofc>9</ofc><rad>&scirc;torm</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sxtorm.0i">
  <kap><tld/>i</kap>
  <snc mrk="sxtorm.0i.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Forte, dan&gcirc;ere <ref tip="super" cel="vent.0i">venti</ref>, 
      eventuale kun pluvo, ne&gcirc;o, sablo...:
      <ekz>
        se super ili <tld/>us la ventego, &ccirc;i tie restus same kviete
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;<tld/>os,&rightquot; <nom>Donald</nom> gruntis moroze
        <fnt>
          <aut>J. Francis</aut>
          <vrk>La Granda Kaldrono</vrk>
          <lok>1978</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la batalo jam <tld/>is viglege
        <fnt>
          <bib>ChV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">st&uuml;rmen</trd>
  <trdgrp lng="es">
    <trd>tempestear</trd>,
    <trd>haber tormenta</trd>
  </trdgrp>
  <trd lng="fr">temp&ecirc;ter</trd>
  <trdgrp lng="uk">
    <trd>&#x431;&#x443;&#x448;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x448;&#x430;&#x43B;&#x435;&#x43D;&#x456;&#x442;&#x438; (&#x43F;&#x440;&#x43E; &#x431;&#x443;&#x440;&#x44E;)</trd>
  </trdgrp>
</drv>

<drv mrk="sxtorm.0a">
  <kap>
    <tld/>a
  </kap>
  <snc mrk="sxtorm.0a.MET">
    <dif>
      Trafata, agitata de <tld/>o:
      <ekz>
        post longa na&gcirc;ado en <tld/>a oceano, jen li trafis florplenan insulon
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la &scirc;ipo]</klr> devis batali kontra&ubreve; <tld/>a vetero
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[li]</klr> veki&gcirc;is el <tld/>a son&gcirc;o
        <fnt>
          <bib>MMa</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">st&uuml;rmisch</trd>
  <trd lng="en">stormy</trd>
  <trd lng="fr">temp&ecirc;tueux</trd>
  <trdgrp lng="uk">
    <trd>&#x431;&#x443;&#x440;&#x44F;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x431;&#x443;&#x440;&#x435;&#x43C;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x431;&#x443;&#x440;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x431;&#x443;&#x440;&#x445;&#x43B;&#x438;&#x432;&#x438;&#x439;</trd>,
    <trd>&#x448;&#x442;&#x43E;&#x440;&#x43C;&#x43E;&#x432;&#x438;&#x439;</trd>
  </trdgrp>
</drv>


<drv mrk="sxtorm.0o">
  <kap><ofc>9</ofc><tld/>o  <fnt><aut>M. Rollet de l'Isle</aut>,
  <vrk>Provo de marista terminaro</vrk>, <lok>1908</lok></fnt></kap>
  <snc mrk="sxtorm.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Forta, dan&gcirc;era
      <ref tip="vid" cel="vent.0ego.MET">ventego</ref>:
      <ekz>
        la fulmotondro i&gcirc;is uragana <tld/>o
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        dikegaj uloj kun kurbaj kaj nodoplenaj bran&ccirc;egoj kiuj dum jarcentoj kaj jarmilo
        eltenis <tld/>ojn
        <fnt>
          <bib>ViV</bib>
        </fnt>;
      </ekz>
      <ekz>
        sova&gcirc;a <tld/>o pelis la pezajn nubojn kiel paperpecojn sur la nokta &ccirc;ielo
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="tondr.fulmo0o">fulmotondro</ref>,
       <ref cel="tempes.0o">tempesto</ref>,
       <ref cel="uragan.0o.MET">uragano<sncref/></ref>
    </refgrp>
  </snc>
  <snc mrk="sxtorm.0o.atako">
    <dif>
      Rapida kaj forta <ref tip="super" cel="atak.0o">atako</ref>:
      <ekz>
        <tld/>o de la indigno kaptas subite la malgrandan salonon
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de apla&ubreve;dado, kvaza&ubreve; bruego de tertremo, ekskuis la placon
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>oj de la vivo
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        Esperanto rezistis la <tld/>ojn kaj sangan elprovon sub fa&scirc;ismo
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        la psika <tld/>o subite paci&gcirc;is, kiam &scirc;i levis la rigardon <klr>[&#x2026;]</klr> kaj
        amba&ubreve; samsekunde ridetis
        <fnt>
          <bib>ChM</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_t;&c_o;&c_r;&c_m;</trd>
  <trd lng="bo">&t_ra;&t_lata;&t_shabchu;&t_nga;&t_tsheg;&t_-a;&t_tsha;&t_shabchu;&t_ba;&t_tsheg;</trd>
  <trd lng="de">Sturm</trd>
  <trd lng="el">&theta;&ypsilon_ton;&epsilon;&lambda;&lambda;&alfa;
  </trd>
  <trd lng="en">storm</trd>
  <trdgrp lng="es">
    <trd>tormenta</trd>
    <trd>tempestad</trd>
  </trdgrp>
  <trd lng="fr">temp&ecirc;te</trd>
  <trd lng="hu">sz&eacute;lvihar</trd>
  <trdgrp lng="it">
    <trd>temporale<klr> (di vento)</klr></trd>,
    <trd>tempesta<klr> (di vento, di sabbia, ecc...)
  </klr></trd>
  </trdgrp>
  <trd lng="nl">storm</trd>
  <trd lng="pt">tempestade</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_t;&c_o;&c_r;&c_m;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_ja;</trd>
  </trdgrp>
  <trd lng="sv">storm</trd>
  <trdgrp lng="uk">
    <trd>&#x431;&#x443;&#x440;&#x44F;</trd>,
    <trd>&#x431;&#x443;&#x440;&#x430;&#x43D;</trd>,
    <trd>&#x448;&#x442;&#x43E;&#x440;&#x43C;</trd>
  </trdgrp>
</drv>

<drv mrk="sxtorm.kirlo0o">
  <kap>kirlo<tld/>o</kap>
  <snc mrk="sxtorm.kirlo0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      <ref tip="super" cel="sxtorm.0o.MET">&Scirc;tormo<sncref/></ref>
      kiu vortice turni&gcirc;as:
      <ekz>
        dum neniu anta&ubreve;a jaro <ind>kirlo<tld/>o</ind>j estis tiel fortaj, &gcirc;is 280 km/h rapidegaj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2005/008822.php">Du mil kvin</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="ciklon.0o.MET">ciklono</ref>
  </refgrp>
  <trd lng="de">Wirbelsturm</trd>
  <trd lng="fr">tornade</trd> 
  <trd lng="uk">&#x446;&#x438;&#x43A;&#x43B;&#x43E;&#x43D;</trd>
</drv>

<drv mrk="sxtorm.negxo0o">
  <kap>ne&gcirc;o<tld/>o</kap>
  <snc mrk="sxtorm.negxo0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      <ref tip="super" cel="sxtorm.0o.MET">&Scirc;tormo<sncref/></ref>
      kiu kunpelas ne&gcirc;on:
      <ekz>
        ofte ciklonoj alportas ne&gcirc;egon kaj <ind>ne&gcirc;o<tld/>o</ind>jn
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2015/011497.php">Ekstremaj veter-fenomenoj obli&gcirc;as</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Schneesturm</trd>
  <trd lng="fr">temp&ecirc;te de n&ecirc;ge</trd> 
  <trdgrp lng="uk">
    <trd>&#x431;&#x443;&#x440;&#x430;&#x43D;</trd>,
    <trd>&#x441;&#x43D;&#x456;&#x433;&#x43E;&#x432;&#x456;&#x439;</trd>,
    <trd>&#x445;&#x443;&#x433;&#x43E;&#x432;&#x456;&#x439;</trd>,
    <trd>&#x445;&#x443;&#x433;&#x430;</trd>
  </trdgrp>
</drv>

<drv mrk="sxtorm.sabl0o">
  <kap>sablo<tld/>o</kap>
  <snc mrk="sxtorm.sabl0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      <ref tip="super" cel="sxtorm.0o.MET">&Scirc;tormo<sncref/></ref>,
      ventego kiu kunportas sablon kaj polvon:
      <ekz>
        de pratempoj la sablo<tld/>o &scirc;lif<klr>[i]</klr>s iliajn e&gcirc;ojn kaj malglata&jcirc;ojn
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Goabo, la dezerto de la Koloroj</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni povus &gcirc;uste imagi la bezonon de vualo kiel masko kontra&ubreve; sablo-<tld/>oj kaj
        brula varmo sub la dezerta suno
        <fnt>
          <bib>Monato</bib>
          <aut>H. Al-Amily</aut>
          <vrk><url ref="&Monato;/2007/009296.php">La vualo, romantika kaj erotika</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="tromb.0o.sablo">trombo<sncref/></ref>
  </snc>
  <trd lng="de">Sandsturm</trd>
  <trd lng="es">tormenta<klr> (de arena)</klr></trd> 
  <trd lng="fr">temp&ecirc;te de sable</trd> 
  <trd lng="it"><ind>tempesta</ind> di sabbia</trd>
  <trd lng="uk">&#x43F;&#x456;&#x449;&#x430;&#x43D;&#x430; &#x431;&#x443;&#x440;&#x44F;</trd>
</drv>
</art>
