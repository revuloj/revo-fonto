    <trd>cz&lstroke;onek &lstroke;awy <ind>przysi&eogonek;g&lstroke;ych
    </ind></trd>,
    <trd>juror</trd>,
    <trd>cz&lstroke;onek <ind>jury</ind></trd>
  </trdgrp>
  <trd lng="pt">jurado</trd>
  <trdgrp lng="uk">
    <trd>&#1087;&#1088;&#1080;&#1089;&#1103;&#1078;&#1085;&#1080;&#1081; &#1079;&#1072;&#1089;&#1110;&#1076;&#1072;&#1090;&#1077;&#1083;&#1100;</trd>,
