    <trd>&#29956;&#36873;&#23567;&#32452; <pr>zh&#275;nxu&#462;nxi&#462;oz&#468;</pr></trd>,
    <trd>&#23457;&#26597;&#22996;&#21592;&#20250; <pr>sh&#283;nch&#225;w&#283;iyu&#225;nhu&#236;</pr></trd>,
    <trd>&#35413;&#23529;&#22996;&#21729;&#26371; <pr>p&#237;ngsh&#283;nw&#283;iyu&#225;nhu&#236;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="jxuri.0ano">
  <kap><tld/>ano</kap>
  <snc mrk="jxuri.0ano.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Membro de <tld/>o:
      <ekz>
        asiza <tld/>ano
        <fnt><bib>PIV2</bib></fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_s;&c_ja;&c_zh;&c_n;&c_y; <klr>(&c_u; &c_s;&c_u;&c_d;&c_z;&c_je;)</klr></trd>,
    <trd>&c_l;&c_a;&c_w;&c_n;&c_ib;&c_k; <klr>(&c_u; &c_s;&c_u;&c_d;&c_z;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="ca">jurat<klr> (membre d'un jurat)</klr></trd>
  <trd lng="fa">&a_ayn;&a_D;&a_w; <ind>&a_h;&f_y;&a_y_hamza;&a_t; &a_m;&a_n;&a_S;&a_f;&a_h;</ind></trd>
