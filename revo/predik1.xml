<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: predik1.xml,v 1.28 2023/10/20 16:38:17 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>predikat</rad>/o
    </kap>

    <drv mrk="predik1.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <uzo tip="fak">GRA</uzo>
      <snc mrk="predik1.0o.GRA">
	<dif>
	  <ref tip="super" cel="fraz.0elemento">Frazmembro</ref>
          esprimanta la agon a&ubreve; staton de la
          <ref cel="subjek.0o.GRA">subjekto<sncref/></ref>:
	  <ekz>
            En la frazo: <ctl>La patro amas sian filon</ctl> la vorto
            <ctl>patro</ctl> estas subjekto, la vorto <ctl>amas</ctl>
            estas <tld/>o
            <fnt><bib>VdE</bib></fnt>;
          </ekz><ekz>
	    en la frazoj <ctl>li sidi&gcirc;is</ctl>,
	    <ctl>&scirc;i &scirc;ajnas juna</ctl>, <ctl>for la ne&ubreve;tralismon</ctl>,
	    la vortoj <ctl>sidi&gcirc;is</ctl>, <ctl>&scirc;ajnas juna</ctl>,
            <ctl>for</ctl> estas <tld/>oj; plej ofte la <tld/>on
            esprimas verbo<fnt><bib>PIV1</bib></fnt>.
	  </ekz>
	</dif>
	<trd lng="be">&c_v;&c_y;&c_k;&c_a;&c_z;&c_mol;&c_n;&c_ib;&c_k;</trd>
        <trd lng="bg">&c_s;&c_k;&c_a;&c_z;&c_u;&c_je;&c_m;&c_o;</trd>
        <trdgrp lng="de">
            <trd><ofc>*</ofc>Pr&auml;dikat</trd>,
            <trd>Satzaussage</trd>
        </trdgrp>
	<trd lng="en">predicate</trd>
	<trd lng="es">predicado</trd>
	<trd lng="fr">pr&eacute;dicat</trd>
	<trd lng="hu">&aacute;ll&iacute;tm&aacute;ny</trd>
	<trd lng="nl">gezegde</trd>
        <trd lng="pl"><ofc>*</ofc>orzeczenie</trd>
        <trd lng="pt">predicado</trd>
	<trd lng="ru"><ofc>*</ofc>&c_s;&c_k;&c_a;&c_z;&c_u;&c_je;&c_m;&c_o;&c_je;</trd>
	<trd lng="sv">predikat</trd>
      </snc>

      <snc mrk="predik1.0o.MAT">
	<uzo tip="fak">MAT</uzo>
	<fnt><bib>MatVort</bib><lok>2123</lok></fnt>
	<dif>
	  <ref cel="propoz.0o.aserto">Propozicio<sncref/></ref>, entenanta
	  variablon:
	  <ekz>
	    <ctl><frm><k>x</k></frm> estas para nombro</ctl>.
	  </ekz>
	</dif>
	<trd lng="be">&c_p;&c_r;&c_e;&c_d;&c_y;&c_k;&c_a;&c_t;</trd>
        <trdgrp lng="de">
            <trd>Pr&auml;dikat</trd>,
            <trd>unges&auml;ttigter Ausdruck</trd>
        </trdgrp>
	<trd lng="en">predicate</trd>
	<trd lng="fr">pr&eacute;dicat</trd>
        <trd lng="hu">predik&aacute;tum</trd>
	<trd lng="pl">predykat</trd>
	<trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_i;&c_k;&c_a;&c_t;</trd>
      </snc>

      <snc>
	<fnt>PV</fnt>
	<uzo tip="stl">EVI</uzo>
	<ref tip="dif" cel="predik2.0o">predikativo</ref>
      </snc>
      <trd lng="cs">p&rcaron;&iacute;sudek</trd>
        <trdgrp lng="de">
            <trd>Pr&auml;dikativum</trd>,
            <trd>Pr&auml;dikatserg&auml;nzung</trd>
        </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8FF0;&#x90E8;  <pr>&#12376;&#12421;&#12388;&#12406;</pr></trd>,
    <trd>&#x8FF0;&#x8A9E;  <pr>&#12376;&#12421;&#12388;&#12372;</pr></trd>,
    <trd>&#x8CD3;&#x8F9E;  <pr>&#12402;&#12435;&#12376;</pr></trd>
  </trdgrp>
  <trd lng="sk">pr&iacute;sudok</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_i;&c_s;&c_u;&c_d;&c_o;&c_k;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_i;&c_k;&c_a;&c_t;</trd>
  </trdgrp>
</drv>
  </art>
