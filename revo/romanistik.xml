<drv mrk="romanistik.0o">
  <kap><tld/>o</kap>
  <snc mrk="romanistik.0o.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      Parto de <ref tip="malprt" cel="lingv.0oscienco.LIN">lingvoscienco</ref>
      kiu studas <ref tip="vid" cel="latin.0idaj_lingvoj.LIN">latinidajn lingvojn</ref>:
      <ekz>
         <nom>GOTOO Hitosi</nom>, nask. 1955 <klr>[&#8230;]</klr> okupi&gcirc;as
         pri &gcirc;enerala lingvistiko, <tld/>o, korpus-lingvistiko, historio de
         lingvistiko kaj esperantologio
         <fnt>
           <aut>Gotoo H.</aut>
           <vrk><url
           ref="https://interlingvistiko.net/wp-content/uploads/2023/07/EES-2023-part-14.pdf">
           Recenzo de la libro Metodoj de interlingva komunikado [&#8230;]</url></vrk>
           <lok>Esperanto Studies, 4(12)</lok>
         </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_m;&c_a;&c_n;&c_ib;&c_s;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="de">Romanistik</trd>
  <trd lng="fr">romanistique</trd>
</drv>
