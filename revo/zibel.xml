<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: zibel.xml,v 1.30 2023/09/05 09:43:57 revo Exp $">
<kap>
 <ofc>*</ofc>
 <rad>zibel</rad>/o
</kap>
<drv mrk="zibel.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="zibel.0o.ZOO">
     <uzo tip="fak">ZOO</uzo>
     <dif>
       Malgranda <ref tip="lst" cel="best.rabo0o" lst="voko:rabobestoj">rabobesto</ref> kun svelta korpo kaj
       mallongaj kruroj, &ccirc;asata pro la tre valora pelto;
       <ref tip="lst" cel="speci.0o.BIO" lst="voko:zoologiaj_specioj">specio</ref>
       de <ref tip="super" cel="martes.0o">marteso</ref>
       (<trd lng="la">Martes zibellina</trd>):
      <ekz>
        tre kara best' estas <tld/>o / pro sia bela varma felo
        <fnt>
          <bib>Fab4</bib>
          <lok>Abocolibro</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum dudek kvar vintroj mi &ccirc;asadis <tld/>ojn kaj mustelojn en la 
        arbaroj de Siberujo
        <fnt>
          <aut>S. S. Blicher, trad. H. J. Bulthuis</aut>,
          <vrk>Taglibro de Vila&gcirc;-pedelo</vrk>,
          <lok>1922</lok>
        </fnt>.
      </ekz>       
    </dif>
    <bld lok="&WCU;/thumb/6/66/Zibellino_del_Barguzin_(2).jpg/640px-Zibellino_del_Barguzin_(2).jpg" prm="CC-BY-SA-3.0"><tld/>o
        <fnt>
          <aut>Francorov &ccirc;e Itala Vikipedio</aut>
          <lok><url ref="&WCW;/File:Zibellino_del_Barguzin_(2).jpg">&WCW;/File:Zibellino_del_Barguzin_(2).jpg</url></lok>
       </fnt>
    </bld>
  </snc>
  <snc mrk="zibel.0o.pelto">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="pelt.0o">Pelto</ref> el la felo de <tld/>o<sncref ref="zibel.0o.ZOO"/>:
      <ekz>
        virinoj, vestitaj per atlaso kaj <tld/>o, certe la posedantinoj de la du kale&scirc;oj
        atendantaj
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_o;&c_b;&c_a;&c_l;&c_mol;</trd>
  <trd lng="ca">mart gibel&iacute;</trd>
  <trd lng="cs">sobol</trd>
  <trd lng="de">Zobel</trd>
  <trd lng="el">&sigma;&alfa;&my;&omikron;&ypsilon_ton;&rho;&jota;</trd>
  <trdgrp lng="en">
     <trd>sable</trd>,
     <trd>marten</trd>
   </trdgrp>
  <trd lng="es">marta cebellina</trd>
  <trd lng="fa">&a_s;&a_m;&a_w;&a_r; &a_s;&f_y;&a_b;&a_r;&f_y;</trd>
  <trd lng="fr">zibeline</trd>
  <trd lng="hu">coboly</trd>
  <trd lng="it">zibelino</trd>
  <trd lng="ja">&#x30AF;&#x30ED;&#x30C6;&#x30F3;</trd>
  <trd lng="nl">sabeldier</trd>
  <trd lng="pl">sob&oacute;l</trd>
  <trdgrp lng="pt">
     <trd>zibelina</trd>,
     <trd>marta-zibelina</trd>
   </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_b;&c_o;&c_l;&c_mol;</trd>
  <trd lng="sk">sobo&lcaron;</trd>
  <trd lng="uk">&#x441;&#x43E;&#x431;&#x43E;&#x43B;&#x44C;</trd>
</drv>
</art>
