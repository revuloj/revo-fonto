<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
  <ofc>*</ofc>
  <rad>zibel</rad>/o
  </kap>
<drv mrk="zibel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="martes.0o">marteso</ref>
      (<trd lng="la">Martes zibellina</trd>),
      rabobesteto kun valora pelto.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_o;&c_b;&c_a;&c_l;&c_mol;</trd>
  <trd lng="de">Zobel</trd>
  <trd lng="el">&sigma;&alfa;&my;&omikron;&ypsilon_ton;&rho;&jota;</trd>
  <trdgrp lng="en">
    <trd>sable</trd>,
    <trd>sable <ind>martin</ind></trd>
  </trdgrp>
  <trd lng="es">marta cebellina</trd>
  <trd lng="hu">coboly</trd>
  <trd lng="nl">sabeldier</trd>
  <trdgrp lng="pt">
    <trd>zibelina</trd>,
    <trd>marta zibelina</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_b;&c_o;&c_l;&c_mol;</trd>
</drv>
</art>
