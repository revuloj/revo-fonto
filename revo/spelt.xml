<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>spelt</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="spelt.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="tritik.0o">tritiko</ref>
      (<trd lng="la">Triticum spelta</trd> a&ubreve; <trd
lng="la">Triticum 
      aestivum</trd>), kulturata en montarecaj
      regionoj kaj kies malgrandaj kaj brunaj grajnoj estas forte
      algluitaj al la grajn&scirc;elo.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_a;&c_v;&c_ib;&c_c;&c_a; <klr
tip="amb">(&c_v;&c_ib;&c_d;
&c_p;&c_sh;&c_a;&c_n;&c_ib;&c_c;&c_y;)</klr></trd>
  <trd lng="cs">pšenice špalda</trd>
  <trdgrp lng="de">
    <trd>Dinkel</trd>,
    <trd>Spelz <klr tip="ind">(Weizen)</klr></trd>
  </trdgrp>
  <trd lng="en">spelt</trd>
  <trd lng="fr">&eacute;peautre</trd>
  <trdgrp lng="hu">
    <trd>t&ouml;nk&ouml;ly</trd>,
    <trd>nagy <ind>alakor</ind></trd>,
    <trd>piros <ind>alakor</ind></trd>,
  </trdgrp>
  <trd lng="pl"><ind>orkisz</ind> pszenny</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_b;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>prstnatec</trd>,
    <trd>pšenica horská</trd>,
    <trd>špalda</trd>
  </trdgrp>
</drv>
</art>
