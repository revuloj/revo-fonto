<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>blazon</rad>/o
</kap>
<drv mrk="blazon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HER</uzo>
  <snc>
    <dif>
      Tuto de la <ref tip="super" cel="insign.0o">insignoj</ref> de nobelo, urbo, regno, gildo.
    </dif>
    <refgrp tip="vid">
      <ref cel="sxild.blazon0o">blazon&scirc;ildo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_r;&c_b;</trd>
  <trd lng="ca">blas&oacute;</trd>
  <trd lng="de">Wappen</trd>
  <trd lng="es">blas&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>armes<klr> (h&eacute;raldique)</klr></trd>,
    <trd>armoiries</trd>,
    <trd>blason</trd>
  </trdgrp>
  <trd lng="hu">c&iacute;mer</trd>
  <trdgrp lng="nl">
    <trd>wapen</trd>,
    <trd>blazoen</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>bras&atilde;o</trd>, 
   <trd>emblema</trd>, 
   <trd>escudo de armas</trd> 
  </trdgrp>
  <trd lng="ru">&c_g;&c_je;&c_r;&c_b;</trd>
</drv>
</art>
