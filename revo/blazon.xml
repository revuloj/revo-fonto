<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>blazon</rad>/o
</kap>
<drv mrk="blazon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HER</uzo>
  <snc>
    <dif>
      Tuto de la <ref tip="super" cel="insign.0o">insignoj</ref> de nobelo, urbo, regno, gildo.
    </dif>
    <refgrp tip="vid">
      <ref cel="sxild.blazon0o">blazon&scirc;ildo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>armes<klr> (h&eacute;raldique)</klr></trd>,
    <trd>armoiries</trd>,
    <trd>blason</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>wapen</trd>,
    <trd>blazoen</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_je;&c_r;&c_b;</trd>
</drv>
</art>
