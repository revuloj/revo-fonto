<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>soldat</rad>/o
    </kap>
    <drv mrk="soldat.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc>
        <dif>
          Malsupera <ref tip="super" cel="milit.0isto">militisto</ref>,
          ano de <ref tip="malprt" cel="arme.0o.sxtata">terarmeo</ref>.
        </dif>
        <rim>
          La vorto ofte aperas <tld/>o kontraste al
          <ref cel="oficir.0o">oficiro</ref>
          (kaj <ref cel="oficir.sub0o">suboficiro</ref>):
          <ekz>
            oficiroj kaj <tld/>oj;</ekz> a&ubreve; al militflotano:
          <ekz>
            <tld/>oj kaj matrosoj.</ekz>          
        </rim>
      </snc>
      <trd lng="de">Soldat</trd>
      <trd lng="en">soldier</trd>
      <trd lng="nl">soldaat</trd>
      <trd lng="ru">&c_s;&c_o;&c_l;&c_d;&c_a;&c_t;</trd>
      <trd lng="sv">soldat</trd>
    </drv>
    <drv mrk="soldat.0ejo">
      <kap><tld/>ejo</kap>
      <snc>
        <dif>
          <ref tip="dif" cel="kazern.0o">Kazerno</ref>.  
        </dif>
      </snc>
      <trd lng="de">Kaserne</trd>
      <trd lng="nl">kazerne</trd>
      <trd lng="ru">&c_k;&c_a;&c_z;&c_a;&c_r;&c_m;&c_a;</trd>
    </drv>
  </art>
