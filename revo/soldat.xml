<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>soldat</rad>/o
</kap>
<drv mrk="soldat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Malsupera <ref tip="super" cel="milit.0isto">militisto</ref>,
      ano de <ref tip="malprt" cel="arme.0o.sxtata">terarmeo</ref>.
    </dif>
    <rim>
      La vorto <tld/>o ofte aperas kontraste al
      <ref cel="oficir.0o">oficiro</ref>
      (kaj <ref cel="oficir.sub0o">suboficiro</ref>):
      <ekz>
        oficiroj kaj <tld/>oj;
      </ekz>
      a&ubreve; al militflotano:
      <ekz>
        <tld/>oj kaj matrosoj.
      </ekz>
    </rim>
  </snc>
  <adm>
    La termino "matroso" aperanta en ekz 
    ne ekzistas en REVO.
    <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_l;&c_d;&c_a;&c_t;</trd>,
    <trd>&c_zh;&c_a;&c_w;&c_n;&c_je;&c_r;</trd>
  </trdgrp>
  <trd lng="de">Soldat</trd>
  <trd lng="en">soldier</trd>
  <trd lng="fr">soldat</trd>
  <trd lng="hu">katona</trd>
  <trd lng="nl">soldaat</trd>
  <trd lng="pl">&zdot;o&lstroke;nierz</trd>
  <trd lng="ru">&c_s;&c_o;&c_l;&c_d;&c_a;&c_t;</trd>
  <trd lng="sv">soldat</trd>
</drv>

<drv mrk="soldat.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <ref tip="dif" cel="kazern.0o">kazerno</ref>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_z;&c_a;&c_r;&c_m;&c_a;</trd>
  <trd lng="de">Kaserne</trd>
  <trd lng="fr">caserne</trd>
  <trd lng="hu">kasz&aacute;rnya</trd>
  <trd lng="nl">kazerne</trd>
  <trd lng="pl">koszary</trd>
  <trd lng="ru">&c_k;&c_a;&c_z;&c_a;&c_r;&c_m;&c_a;</trd>
</drv>

<drv mrk="soldat.rajdo0o">
  <kap>rajdo<tld/>o, rajdo<tld/>o <fnt><bib>PV</bib><lok>kavalerio</lok></fnt></kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      <tld lit="S"/>o servanta sur &ccirc;evalo.
    </dif>
    <ref tip="sin" cel="kavale.0ano">kavaleriano</ref>
  </snc>
  <trd lng="fr">cavalier<klr tip="ind"> (soldat de cavalerie)</klr></trd>
</drv>
</art>
