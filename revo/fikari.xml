<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: fikari.xml,v 1.5 2024/09/16 09:34:11 afido Exp $">
    <kap>
      <rad>fikari</rad>/o
      <fnt><bib>KrDE</bib></fnt>
    </kap>

<drv mrk="fikari.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio <ref tip="malprt" cel="ranunk1.0oj">ranunkolacoj</ref> (<trd
      lng="la">Ficaria</trd>),
      kun korformaj, brilaj, longpetiolaj folioj kaj unuopaj okulfrapaj longtigaj stelformaj floroj,

      orflavaj, kvaza&ubreve; lakitaj:
      <ekz>
        flavaj gageoj kaj <tld/>oj maje buntigas la grundon de boskoj
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2014/011095.php"
              >Tammisaari, urbeto &ccirc;e Balta Maro</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/9/94/Ficaria_verna_2_RF.jpg/640px-Ficaria_verna_2_RF.jpg"
      prm="CC BY 4.0"
      ><tld/>oj (Ficaria verna), Hesujo, Germanujo
        <fnt>
          <aut>Robert Flogaus-Faust, propra verko</aut>
          <lok><url ref="&WCW;/File:Ficaria_verna_2_RF.jpg"
              >&WCW;/File:Ficaria_verna_2_RF.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Scharbockskraut</trd>
    <trd lng="en">Ficaria</trd>
    <trd lng="fr">ficaire</trd>
    <trd lng="ru">&c_ch;&c_i;&c_s;&c_t;&c_ja;&c_k; &c_v;&c_je;&c_s;&c_je;&c_n;&c_n;&c_i;&c_j;</trd>
  </snc>
<trdgrp lng="be">
  <trd>&c_ch;&c_y;&c_s;&c_mol;&c_c;&c_ja;&c_k;</trd>,
  <trd>&c_ch;&c_y;&c_s;&c_t;&c_a;&c_c;&c_je;&c_l;</trd>
</trdgrp>
</drv>

  </art>

