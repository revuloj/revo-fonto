<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: baltik.xml,v 1.17 2023/10/29 19:11:00 revo Exp $">
<kap>
  <rad>Baltik</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="baltik.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="baltik.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="dif" cel="mar.BaltaM0o">Balta Maro</ref>:
      <ekz>
        zono de morenaj alta&jcirc;oj tre proksime al la <tld/>o
        <fnt>
          <vrk><url ref="https://esperantomovado.blogspot.com/2012/03/tagoj-de-teramikeco.html">Tagoj de Teramikeco</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ie proksime ku&scirc;as bordoj de <tld/>o, ie proksime bruas la vivo
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x6CE2;&#x7F57;&#x7684;&#x6D77; <pr>b&#x14D;lu&#xF3;d&#xEC;h&#x1CE;i</pr></trd>
</snc>
  <!-- formon arkaikan traduku per arkaika&jcirc;o se tia ekzistas -->
  <trdgrp lng="be">
    <trd>&c_B;&c_a;&c_l;&c_t;&c_y;&c_k;&c_a;</trd>,
    <trd>&c_B;&c_a;&c_l;&c_t;&c_y;&c_j;&c_s;&c_k;&c_a;&c_je;
    &c_m;&c_o;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">B&agrave;ltic</trd>
  <trdgrp lng="cs">
    <trd>Balt</trd>,
    <trd>Baltik</trd>
  </trdgrp>
  <trd lng="de">Ostsee</trd>
  <trdgrp lng="en">
    <trd>Baltic</trd>,
    <trd>Baltic Sea</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>B&aacute;ltico</trd>,
    <trd>mar <ind>B&aacute;ltico</ind></trd>
  </trdgrp>
  <trd lng="he">&#x5D1;&#x5DC;&#x5D8;&#x5D9;</trd>
  <trd lng="hu">Balti-tenger</trd>
  <trdgrp lng="nl">
    <trd>Oostzee</trd>,
    <trd>Baltische Zee</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>Ba&lstroke;tyk</trd>,
    <trd>Morze Ba&lstroke;tyckie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_B;&c_a;&c_l;&c_t;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_B;&c_a;&c_l;&c_t;&c_i;&c_j;&c_s;&c_k;&c_o;&c_je;
    &c_m;&c_o;&c_r;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>Balt</trd>,
    <trd>Baltik</trd>
  </trdgrp>
</drv>

<drv mrk="baltik.0a">
  <kap><tld lit="b"/>a</kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="baltik.0a.GEOG">
   <dif>
     <ref tip="dif" cel="balt.0a">Balta</ref>:
      <ekz>
        la nobeloj de l' <tld/>aj provincoj
        <fnt><bib>PatrojFiloj</bib>, <lok>&ccirc;apitro 23a, p.
        147a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x5DF4;&#x5C14; <pr>b&#x101;&#x11B;r</pr></trd>
</snc>
<!-- formon arkaikan traduku per arkaika&jcirc;o se tia ekzistas -->
  <trd lng="be">&c_b;&c_a;&c_l;&c_t;&c_y;&c_j;&c_s;&c_k;&c_ib;</trd>
  <trdgrp lng="de">
    <trd>Ostsee-</trd>,
    <trd>baltisch</trd>
  </trdgrp>
  <trd lng="hu">balti</trd>
  <trd lng="nl">Baltisch</trd>
  <trd lng="pl">ba&lstroke;tycki</trd>
  <trd lng="ru">&c_b;&c_a;&c_l;&c_t;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
