<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Baltik</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="baltik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <ref tip="dif" cel="balt.B0aMaro">Balta Maro</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_B;&c_a;&c_l;&c_t;&c_y;&c_k;&c_a;</trd>,
    <trd>&c_B;&c_a;&c_l;&c_t;&c_y;&c_j;&c_s;&c_k;&c_a;&c_je; &c_m;&c_o;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">B&agrave;ltic</trd>
  <trd lng="de">Ostsee</trd>
  <trdgrp lng="en">
    <trd>Baltic</trd>,
    <trd>Baltic Sea</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>B&aacute;ltico</trd>,
    <trd>mar <ind>B&aacute;ltico</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Baltique</trd>,
    <trd>Mer baltique</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_B;&c_a;&c_l;&c_t;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_B;&c_a;&c_l;&c_t;&c_i;&c_j;&c_s;&c_k;&c_o;&c_je; &c_m;&c_o;&c_r;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="baltik.0a">
  <kap><tld lit="b"/>a</kap>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="balt.0a">balta</ref>
      <ekz>
        la nobeloj de l'<tld/>aj provincoj
        <fnt><bib>PatrojFiloj</bib>, <lok>&ccirc;apitro 23a, p. 147a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_l;&c_t;&c_y;&c_j;&c_s;&c_k;&c_ib;</trd>
  <trdgrp lng="de">
    <trd>Ostsee...</trd>,
    <trd>baltisch</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>balte<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_a;&c_l;&c_t;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
