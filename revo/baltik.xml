<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Baltik</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="baltik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <ref tip="dif" cel="balt.B0aMaro">Balta Maro</ref>
  </snc>
  <trdgrp lng="en">
    <trd>Baltic</trd>,
    <trd>Baltic Sea</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Baltique</trd>,
    <trd>Mer baltique</trd>
  </trdgrp>
</drv>

<drv mrk="baltik.0a">
  <kap><tld lit="b"/>a</kap>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="balt.0a">balta</ref>
      <ekz>
        la nobeloj de l'<tld/>aj provincoj
        <fnt><bib>PatrojFiloj</bib>, <lok>&ccirc;apitro 23a, p. 147a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>balte<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>
</art>
