<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bagas.xml,v 1.4 2023/10/27 18:29:20 revo Exp $">
<kap>
  <rad>bagas</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>

<drv mrk="bagas.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="bagas.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Fibra <ref tip="super" cel="rekrem.0o.AGR">rekremento</ref>
      de <ref cel="kan.suker0o">sukerkano</ref>:
      <ekz>
        kiam la <tld/>o estas seka, la laboristoj rekolektas
        &gcirc;in kaj uzas &gcirc;in kiel brula&jcirc;on por hejti la fornon
        en la produktejo
        <fnt>
          <aut>A. Saliba</aut>,
          <vrk><url
          ref="http://esperantomovado.blogspot.com/2012/02/panela-sukerajo.html?_sm_au_=iVVbZfk8T6MHbWn5">
          Panela-sukera&jcirc;o</url></vrk>,
          <lok>2012-02-09</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="melas.0o">melaso</ref>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_g;&c_a;&c_s;&c_a;</trd>
  <trd lng="de">Bagasse</trd>
  <trd lng="fr">bagasse</trd>
  <trd lng="it">bagassa</trd>
</drv>
</art>

