<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: galinag.xml,v 1.5 2024/01/10 07:24:39 afido Exp $">
<kap>
  <rad>galinag</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="galinag.0o">
  <kap><tld/>o</kap>
  <snc mrk="galinag.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="malprt" cel="bird.vad0oj">Vadbirdo</ref>
      kun longa, rekta beko, per kiu &gcirc;i ser&ccirc;as bestetojn en
      koto, malfacile &ccirc;asebla, tre simila al <ref cel="skolop.0o">skolopo</ref> sed pli malgranda;
      monda <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref>
      el familio <ref tip="malprt" cel="skolop2.0oj">skolopedoj</ref>
      (<trd lng="la">Gallinago</trd>), al kiu oni ofte alkalkulas la
      novzelandan genron <trd lng="la">Coenocorypha</trd> kaj la unuspecian
      genron <trd lng="la">Lymnocryptes</trd>:
      <ekz>
        la hundino sin distris per persekutado de birdoj
        dise tra la mar&ccirc;o, okaze &gcirc;i kaptis ralon a&ubreve; <tld/>on,
        kaj man&gcirc;is la kapta&jcirc;on
        <fnt>
          <aut>H. Laxness, trad. B. Ragnarsson</aut>
          <vrk>Sendependaj homoj</vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/d/d5/Gallinago_gallinago_a1.JPG/819px-Gallinago_gallinago_a1.JPG" prm="CC
    BY-SA 3.0"><tld/>o (Gallinago gallinago), Japanujo
        <fnt>
          <aut>Alpsdake, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Gallinago_gallinago_a1.JPG">&WCW;/File:Gallinago_gallinago_a1.JPG</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="de">
      <trd>Bekassine</trd>,
      <trd>Sumpfschnepfe</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_k;&c_a;&c_s;</trd>,
    <trd>&c_ch;&c_e;&c_p;&c_ib;&c_k;</trd>,
    <trd>&c_p;&c_ja;&c_k;&c_u;&c_r;</trd>,
    <trd>&c_b;&c_a;&c_r;&c_a;&c_n;&c_ch;&c_y;&c_k; <klr tip='amb'>(&c_p;&c_t;&c_u;&c_sh;&c_k;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="fr">b&eacute;cassine</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&c_r;&c_a;&#x43D;&c_je;&c_c;&#x44C; &c_z;&c_v;&c_i;&c_ch;&c_a;&c_j;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_a;&c_r;&c_a;&#x43D;&c_je;&c_c;&#x44C;</trd>
  </trdgrp>
</drv>
</art>

