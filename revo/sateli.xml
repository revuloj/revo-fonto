<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>satelit</rad>/o
  <fnt>B</fnt>
</kap>
<drv mrk="sateli.0o">
  <kap><tld/>o</kap>
  <snc mrk="sateli.0o.planedo">
    <uzo tip="fak">AST</uzo>
    <dif>
      Mal&ccirc;efa dependa <ref tip="super" cel="planed.0o">planedo</ref>
      rondiranta &ccirc;irka&ubreve; alia &ccirc;efa planedo:
      <ekz>
        la luno estas <tld/>o de la tero.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>hold</trd>,
      <trd>mell&eacute;kbolyg&oacute;</trd>,
    </trdgrp>
    <trd lng="ru">&c_s;&c_p;&c_u;&c_t;&c_n;&c_i;&c_k;</trd>
  </snc>
  <snc mrk="sateli.0o.fihelpanto">
    <dif>
      Fihelpanto tute dependanta de iu; nedisi&gcirc;a fikunulo, al kiu oni
      komisias la krimajn a&ubreve; malnoblajn taskojn.
    </dif>
    <trd lng="hu">cimbora</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_i;&c_h;&c_v;&c_o;&c_s;&c_t;&c_je;&c_n;&c_mol;</trd>,
      <trd>&c_s;&c_a;&c_t;&c_je;&c_l;&c_l;&c_i;&c_t;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sateli.0o.sputniko">
    <dif>
      = <ref tip="dif" cel="sputnik.0o">Sputniko</ref>.
    </dif>
    <trd lng="ru">&c_s;&c_p;&c_u;&c_t;&c_n;&c_i;&c_k;</trd>
  </snc>
  <trdgrp lng="de">
    <trd>Trabant</trd>,
    <trd>Satellit</trd>
  </trdgrp>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_a;&c_d;&c_a;&c_r;&c_o;&c_zh;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_s;&c_a;&c_t;&c_e;&c_l;&c_ib;&c_t;</trd>
  </trdgrp>
  <trd lng="en">satellite</trd>
  <trd lng="es">sat&eacute;lite</trd>
  <trd lng="hu">m&udblac;hold</trd>
  <trd lng="nl">satelliet</trd>
  <trdgrp lng="zh">
    <trd>&#21355;&#26143;</trd>,
    <trd><klr>tr. </klr>&#34907;&#26143;<klr> [w&egrave;ix&#299;ng]</klr></trd>
  </trdgrp>
</drv>
</art>
