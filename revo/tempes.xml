<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>tempest</rad>/o
  <fnt>G</fnt>
</kap>
<drv mrk="tempes.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MET</uzo>
    <dif>
      <ref tip="dif" cel="sxtorm.0o">&scirc;tormo</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_u;&c_r;&c_a;</trd>, 
    <trd>&c_n;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_c;&c_a;</trd>, 
    <trd>&c_sh;&c_t;&c_o;&c_r;&c_m;</trd>
  </trdgrp>  
  <trd lng="de">Sturm</trd>
  <trdgrp lng="en">
     <trd>storm</trd>,
     <trd>tempest</trd>
  </trdgrp>
  <trdgrp lng="es">
     <trd>tormenta</trd>,
     <trd>tempestad</trd>
  </trdgrp>
  <trd lng="hu">vihar</trd>
  <trd lng="nl">storm</trd>
  <trd lng="ru">&c_b;&c_u;&c_r;&c_ja;</trd>
</drv>
</art>
