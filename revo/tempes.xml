<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tempes.xml,v 1.20 2020/11/14 12:11:39 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>tempest</rad>/o 
    <fnt>
      <aut>A. Grabowski</aut>,
      <vrk>Slownik jezyka Esperanto</vrk>, <lok>[1910-1916?]</lok>
    </fnt>
    <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tempes.0o">
  <kap><tld/>o</kap>
  <snc mrk="tempes.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Tre <ref tip="vid" cel="sxtorm.0o">&scirc;torm</ref>a vetero,
      ofte kun pluvo, ne&gcirc;o a&ubreve; sablo (en dezertoj):
      <ekz>
        inter montoj surprizis lin ega <tld/>o
        <fnt><bib>RabistFab</bib></fnt>;
      </ekz>
      <ekz>
        somera <tld/>o torentis sur la monteton
        <fnt>
          <bib>Monato</bib>
          <aut>J&uacute;lia Sigmond</aut>
          <vrk><url ref="https://www.monato.be/2009/009693.php">La mistera nokto</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj forrapidis, ja la <tld/>o i&gcirc;is pli kaj pli neeltenebla
        <fnt>
          <bib>Monato</bib>
          <aut>Julia Sigmond</aut>
          <vrk><url ref="https://www.monato.be/2015/011386.php">Cent fabeloj</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i-jare tutmonde estis aparte multaj &scirc;tormegoj, <tld/>oj, uraganoj kaj precipe
        tajfunoj, ciklonoj kaj tornadoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2004/008474.html">Koincido</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_u;&c_r;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_c;&c_a;</trd>,
    <trd>&c_sh;&c_t;&c_o;&c_r;&c_m;</trd>
  </trdgrp>
  <trd lng="cs">bou&rcaron;e</trd>
  <trdgrp lng="de">
    <trd>Unwetter</trd>,
    <trd>st&uuml;rmisches <ind>Wetter</ind></trd>,
    <trd>Sturm</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>storm</trd>,
    <trd>tempest</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>tormenta</trd>,
    <trd>tempestad</trd>
  </trdgrp>
  <trd lng="fr">temp&ecirc;te</trd>
  <trd lng="hu">vihar</trd>
  <trd lng="nl">storm</trd>
  <trdgrp lng="pl">
    <trd>burza</trd>,
    <trd>nawa&lstroke;nica</trd>,
    <trd>wichura</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>tempestade</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_u;&c_r;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>ve&lcaron;k&aacute; b&uacute;rka</trd>,
    <trd>v&iacute;chrica</trd>
  </trdgrp>
</drv>
</art>
