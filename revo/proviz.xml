<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>proviz</rad>/i
</kap>
<drv mrk="proviz.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="proviz.0i.havigi">
    <dif>
      Havigi, la&ubreve; pli malpli granda kvanto, tion, kion oni povas
      bezoni:
      <ekz>
        <tld/>i armeon per armiloj;
      </ekz>
      <ekz>
        <tld/>i kelon je vino<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i iun je &ccirc;io necesa<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i &gcirc;ardenon per akvo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li <tld/>adis al ili panon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        sin <tld/>i je &ccirc;evalo por forkuri;
      </ekz>
      <ekz>
        multaj presejoj jam <tld/>i&gcirc;is je niaj supersignitaj
        literoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        domo bone <tld/>ita per nutra&jcirc;o;
      </ekz>
      <ekz>
        kiu lernas esperanton, sin <tld/>as per plia armilo por la
        monda konkurado por vivo;
      </ekz>
      <ekz>
        ne forgesu <tld/>i lin per lia oficiala mandato;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i la gazetojn per sciigoj pri la Esperanta
        movado;
      </ekz>
      <ekz>
        en<tld/>i sciojn en sia memoro.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekip.0i">ekipi</ref>
    </refgrp>
    <trd lng="de">versorgen</trd>
  </snc>
  <snc mrk="proviz.0i.kompletigi">
    <dif>
      Almeti ion al objekto por &gcirc;in kompletigi (garni 1, 2):
      <ekz>
        <tld/>i bibliotekon per libroj, fenestron per vitroj;
      </ekz>
      <ekz>
        &ccirc;i tiu kvartalo estis <tld/>ita per fortaj muroj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la apogilo de la se&gcirc;oj supre estis <tld/>ita per
        tabuleto<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kvarangula, per la ritpenikoj <tld/>ita tuko<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="garn.0i.plifortigi">garni <sncref/></ref>,
      <ref cel="garn.0i.kompletigi">garni <sncref/></ref>
    </refgrp>
    <trd lng="de">versehen</trd>
  </snc>
  <snc mrk="proviz.0i.prizorgi">
    <dif>
      Prizorgi:
      <ekz>
        <tld/>i la bezonojn<fnt>B</fnt>;
      </ekz>
      <ekz>
        la bezonoj estis malavare <tld/>itaj<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">provide</trd>
  <trd lng="es">proveer</trd>
  <trdgrp lng="fr">
    <trd>doter <klr>(fournir)</klr></trd>,
    <trd>fournir</trd>,
    <trd>pourvoir</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ell&aacute;t</trd>,
    <trd>felszerel</trd>
  </trdgrp>
  <trd lng="nl">voorzien van</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_n;&c_a;&c_b;&c_zh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_n;&c_a;&c_b;&c_d;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="proviz.0o">
  <kap><tld/>o</kap>
  <snc mrk="proviz.0o.ado">
    <ref tip="dif" cel="proviz.0ado"><tld/>ado</ref>
  </snc>
  <snc mrk="proviz.0o.ajxo">
    <ref tip="dif" cel="proviz.0ajxo"><tld/>a&jcirc;o</ref>
  </snc>
  <trd lng="es">provisi&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>approvisionnement</trd>,
    <trd>provision</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ell&aacute;t&aacute;s</trd>,
    <trd>felszerel&eacute;s</trd>,
    <trd>ell&aacute;tm&aacute;ny</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>voorraad</trd>,
    <trd>provisie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_n;&c_a;&c_b;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_s;&c_y;</trd>
  </trdgrp>
</drv>

<drv mrk="proviz.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        prizorgi la <tld/>adon de ta&ccirc;mento;
      </ekz>
      <ekz>
        <tld/>ado al la gazetaro per eksterlandaj artikoloj.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">provisi&oacute;n</trd>
  <trd lng="fr">approvisionnement</trd>
  <trdgrp lng="hu">
    <trd>ell&aacute;t&aacute;s</trd>,
    <trd>felszerel&eacute;s</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_n;&c_a;&c_b;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="proviz.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Abunda materialo a&ubreve;
      sufi&ccirc;e granda kolekto da objektoj, kiujn oni povas bezoni:
      <ekz>
        prepari <tld/>ojn<fnt>B</fnt>;
      </ekz>
      <ekz>
        li donis al ili <tld/>ojn por la vojo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        observi &ccirc;u sufi&ccirc;os la <tld/>o de panbiskvitoj kaj
        viando<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        facila akirado de <tld/>o da vortoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ri&ccirc;a <tld/>o da ideoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la soluleco amasigis en &scirc;ia kapo <tld/>on da pensoj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">provisi&oacute;n</trd>
  <trd lng="fr">provision</trd>
  <trdgrp lng="hu">
    <trd>ell&aacute;tm&aacute;ny</trd>,
    <trd>felszerel&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>voorraad</trd>,
    <trd>provisie</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_p;&c_a;&c_s;&c_y;</trd>
</drv>

<drv mrk="proviz.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="proviz.0ejo.loko">
    <dif>
      Loko por konservi <tld/>a&jcirc;ojn:
      <ekz>
        plenigi la <tld/>ejojn per greno;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>ejo de la menso<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">r&eacute;serve <klr>(lieu)</klr></trd>
  <trd lng="hu">rakt&aacute;r</trd>  
  <trdgrp lng="nl">
    <trd>provisiekast</trd>,
    <trd>voorraadkamer</trd>,
    <trd>voorraadschuur</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_k;&c_l;&c_a;&c_d;</trd>
</drv>

<drv mrk="proviz.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="proviz.0umi.MIL">
    <uzo tip="fak">MIL</uzo>
    <ref tip="dif" cel="provia.0i">provianti</ref>
  </snc>
  <trd lng="fr">approvisionner</trd>
  <trdgrp lng="hu">
    <trd>&eacute;lelmez</trd>,
    <trd>&eacute;lelemmel ell&aacute;t</trd>
  </trdgrp>
  <trd lng="nl">provianderen</trd>
</drv>
</art>
