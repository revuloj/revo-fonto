  <trdgrp lng="be">
    <trd>&c_u; &c_ja;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_ib; <ind>&c_z;&c_a;&c_b;&c_je;&c_s;&c_mol;&c_p;&c_ja;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_ja;</ind></trd>,
