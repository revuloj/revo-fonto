  <snc mrk="proviz.0i.kompletigi">
    <dif>
      <ref tip="super" cel="met.al0i">Almeti</ref> ion al objekto por &gcirc;in kompletigi
      <klr>(<ref tip="sub" cel="garn.0i.plifortigi">garni<sncref/></ref> kaj
      <ref tip="sub" cel="garn.0i.kompletigi"><sncref/></ref>)</klr>:
      <ekz>
        potoj, <tld/>itaj per &scirc;nuroj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i bibliotekon per libroj, fenestron per vitroj;
      </ekz>
      <ekz>
        &ccirc;i tiu <klr>[kvartalo]</klr> estis <tld/>ita per fortaj muregoj
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvarangula, per la le&gcirc;aj ritpenikoj provizita tuko
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        se&gcirc;oj, kies apogilo supre estis <tld/>ita per pendanta tabuleto, kiu povis tre
        oportune esti levata, por meti sur &gcirc;in la pre&gcirc;olibron
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tuta korpo, per la artikoj kaj ligiloj <tld/>ita kaj kunigita
        <fnt>
          <bib>NT</bib>
          <lok>Koloseanoj 2:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la programo]</klr> &Ccirc;apelilo  estas <tld/>ita per tre ampleksa helpsistemo
        <fnt>
            <aut>Pa&ubreve;l Peeraerts</aut>
            <bib>Monato</bib>
            <vrk><url ref="&Monato;/1995/A950501.php">Ni levu la &ccirc;apelon!</url></vrk>
            <lok>1995/05, p. 16</lok>
          </fnt>;
      </ekz>
      <ekz>
        la religio <klr>[&#x2026;]</klr> <tld/>is pretekston por perforto en Bosnio, Kosovo, &Ccirc;e&ccirc;enio kaj
        Indonezio
        <fnt>
       <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2000/006847.php">Sobraj pensoj de sobrulo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 22</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="garn.0i.plifortigi">garni <sncref/></ref>,
      <ref cel="garn.0i.kompletigi">garni <sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>ausstatten</trd>,
      <trd>versehen mit</trd>
    </trdgrp>
  </snc>
  <snc mrk="proviz.0i.prizorgi">
    <uzo tip="stl">RAR</uzo>
    <dif>
      <klr>(bezonon)</klr> <ref tip="super" cel="zorg.pri0i">Prizorgi</ref>:
