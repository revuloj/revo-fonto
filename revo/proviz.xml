<drv mrk="proviz.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="proviz.0i.havigi">
    <dif>
      <ref tip="super" cel="hav.0igi">Havigi</ref>, la&ubreve; pli malpli granda kvanto, tion, kion oni povas
      bezoni:
      <ekz>
        ili iris kaj <tld/>is sin per man&gcirc;a&jcirc;oj
        <fnt>
          <bib>MT</bib>
          <lok>Josuo 9:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        apud nia enirejo trovi&gcirc;as delikataj fruktoj &ccirc;iuspecaj, <klr>[&#x2026;]</klr> kiujn mi <tld/>is por
        vi, ho mia amato
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 7:13</lok>
        </fnt>!
      </ekz>
      <ekz>
        li <tld/>adis al ili panon <klr>[&#x2026;]</klr> en tiu jaro
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 47:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u al vi akvon por la tempo de sie&gcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Na&hcirc;um 3:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        dekmila korpuso, preta por la batalo, provizita per <klr>[&#x2026;]</klr> militaj ma&scirc;inoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia kuirejo kaj kelo estas bone <tld/>itaj
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        tendo <klr>[&#x2026;]</klr> <tld/>ita je la plej belaj kusenoj
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>as la fi&scirc;hokojn per loga&jcirc;o
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        akvo<tld/>i la &gcirc;ardenon
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 2:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        plej granda parto de la presejoj <klr>[&#x2026;]</klr> <tld/>i&gcirc;os je nia alfabeto
        <fnt>
          <bib>LR</bib>
          <lok>1. Alfabeto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekip.0i">ekipi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>ausstatten</trd>,
