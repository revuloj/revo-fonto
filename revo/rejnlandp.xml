<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
   <kap>
    <rad>Rejnland-Palatinat</rad>/o
   </kap>
   <drv mrk="rejnlandp.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">GEOG</uzo>
    <snc>
       <dif>
      Unu el la federaciaj &scirc;tatoj de
      <ref tip="malprt" cel="german1.G0ujo">Germanujo</ref>, situanta en la
sudokcidenta parto inter  <ref tip="vid" cel="franc.F0ujo">Francujo</ref>,
Nordrejn-Vestfalujo kaj la rivero <ref tip="vid" cel="rejn.0o">Rejno</ref>.
      Regurbo kaj plej granda urbo estas <ref tip="vid"
cel="majenc.0o">Majenco</ref>.
       </dif>
       <trd lng="de">Rheinland-Pfalz</trd>
       <trd lng="es">Renania-Palatinado</trd>
       <trd lng="hu">Rajnavid&eacute;k-Pfalz</trd>       
    </snc>
</drv>

  <drv mrk="rejnlandp.0ano">
    <kap><tld lit="r"/>ano</kap>
    <snc>
      <dif>
         Civitano de Rejnland-Palatinato.
      </dif>
    </snc>
    <trdgrp lng="de">
       <trd>Rheinland-Pf&auml;lzer</trd>,
       <trd>Pf&auml;lzer</trd>
    </trdgrp>
    <trd lng="hu">rajna-pfalzi</trd>    
  </drv>

  <drv mrk="rejnlandp.0a">
    <kap><tld lit="r"/>a</kap>
    <snc>
      <dif>
         Rilatanta al Rejnland-Palatinato.
      </dif>
    </snc>
    <trdgrp lng="de">
       <trd>rheinland-pf&auml;lzisch</trd>,
       <trd>pf&auml;lzisch</trd>
    </trdgrp>
    <trd lng="hu">rajna-pfalzi</trd>     
  </drv>

  </art>

