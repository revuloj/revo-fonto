<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pat.xml,v 1.31 2024/02/16 19:24:12 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pat</rad>/o
</kap>

<drv mrk="pat.0o">
  <kap><tld/>o</kap>
  <snc mrk="pat.0o.KUI">
  <uzo tip="fak">KUI</uzo>
    <dif>
      Malprofunda <ref tip="super" cel="kasero.0o">kaserolo</ref> kun longa tenilo, 
      servanta por <ref cel="frit.0i">friti</ref>:
      <ekz>
        &ccirc;iu farunofero, kiu estas bakita en forno a&ubreve; pretigita en kaserolo a&ubreve; sur
        <tld/>o, apartenu al la pastro
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 7:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni portis <klr>[&#x2026;]</klr> kradojn por la fornoj, turnrostilojn, dutenilajn
        potojn kaj platajn <tld/>ojn kun kovriloj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i prenis la <tld/>on, kaj elskuis anta&ubreve; li, sed li ne volis man&gcirc;i
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 13:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        grandanombra kolekto da brilantaj kovriloj de potoj, pure frotitaj <tld/>oj,
        poluritaj pladoj
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        li elprenis sangokolbason, &gcirc;in fritis sur <tld/>o
        <fnt>
          <aut>Albert Camus, trad. Michel Duc Goninaz</aut>,
          <vrk>La Fremdulo</vrk>,
          <lok>S.A.T., 1993, p. 40</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="frit.0ilo">fritilo</ref>
    <trd lng="th">&#xE01;&#xE23;&#xE30;&#xE17;&#xE30;</trd>
  </snc>
  <ref tip="hom" cel="pat1.0o">pato (&scirc;ako)</ref>
  <trd lng="be">&c_p;&c_a;&c_t;&c_e;&c_l;&c_mol;&c_n;&c_ja;</trd>
  <trd lng="bg">&c_t;&c_i;&c_g;&c_a;&c_n;</trd>
  <trd lng="bo">&t_ha;&t_tsheg;&t_ya;&t_nga;&t_tsheg;</trd>
  <trdgrp lng="br">
    <trd>paelon-lostek</trd>,
    <trd>palarenn</trd>,
    <trd>pillig-lostek</trd>
  </trdgrp>
  <trd lng="ca">paella</trd>
  <trdgrp lng="cs">
    <trd>pek&aacute;&ccaron;</trd>,
    <trd>p&aacute;nev</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Pfanne</trd>,
    <trd>Tiegel</trd>
  </trdgrp>
  <trd lng="el">&tau;&eta;&gamma;&alfa_ton;&ny;&jota;</trd>
  <trd lng="en">pan</trd>
  <trd lng="es">sart&eacute;n</trd>
  <trd lng="fr">po&ecirc;le</trd>
  <trd lng="hu">serpeny&odblac;</trd>
  <trdgrp lng="ja">
    <trd>&#x30B9;&#x30C6;&#x30FC;&#x30EB;&#x30E1;&#x30A4;&#x30C8;</trd>,
    <trd>&#x624B;&#x8A70;&#x307E;&#x308A;  <pr>&#x3066;&#x3065;&#x307E;&#x308A;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>pan</trd>,
    <trd>braadpan</trd>
  </trdgrp>
  <trd lng="pl">patelnia</trd>
  <trd lng="pt">frigideira</trd>
  <trd lng="ro">tigaie </trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_o;&c_v;&c_o;&c_r;&c_o;&c_d;&c_a;</trd>,
    <trd>&c_s;&c_k;&c_o;&c_v;&c_o;&c_r;&c_o;&c_d;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>panvica</trd>,
    <trd>pek&aacute;&ccaron;</trd>
  </trdgrp>
  <trd lng="uk">&#x441;&#x43A;&#x43E;&#x432;&#x43E;&#x440;&#x43E;&#x434;&#x430;</trd>
  <trdgrp lng="zh">
    <trd>&#x5E73;&#x934B; <pr>p&#xED;nggu&#x14D;</pr></trd>,
    <trd>&#x5E73;&#x9505; <pr>p&#xED;nggu&#x14D;</pr></trd>,
    <trd>&#x5E73;&#x5E95;&#x934B; <pr>p&#xED;ngd&#x1D0;gu&#x14D;</pr></trd>,
    <trd>&#x5E73;&#x5E95;&#x9505; <pr>p&#xED;ngd&#x1D0;gu&#x14D;</pr></trd>,
    <trd>&#x714E;&#x934B; <pr>ji&#x101;ngu&#x14D;</pr></trd>,
    <trd>&#x714E;&#x9505; <pr>ji&#x101;ngu&#x14D;</pr></trd>,
    <trd>&#x5769;&#x57DA; <pr>g&#x101;ngu&#x14D;</pr></trd>,
    <trd>&#x5769;&#x581D; <pr>g&#x101;ngu&#x14D;</pr></trd>
  </trdgrp>
</drv>
</art>
