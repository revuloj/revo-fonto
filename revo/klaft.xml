<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>klaft</rad>/o
</kap>
<drv mrk="klaft.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Malnova mezurunuo de longo a&ubreve; profundo
      (franca: 195 cm; germana: 183 cm; rusa: 213 cm).
    </dif>
    <trd lng="de">Klafter</trd>
    <trd lng="ru">&c_s;&c_a;&c_zh;&c_je;&c_n;&c_mol;</trd>
  </snc>
  <snc num="2">
    <dif>
      Angla mezurunuo de longo (183 cm).
    </dif>
    <trd lng="de">Faden</trd>
    <trdgrp lng="ru">
      <trd>&c_f;&c_a;&c_t;&c_o;&c_m;</trd>,
      <trd>&c_m;&c_o;&c_r;&c_s;&c_k;&c_a;&c_ja; &c_s;&c_a;&c_zh;&c_je;&c_n;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="en">fathom</trd>
  <trd lng="nl">vadem</trd>
</drv>
</art>
