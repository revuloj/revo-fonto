<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: klaft.xml,v 1.23 2024/01/25 09:24:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>klaft</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="klaft.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="klaft.0o.MEZ">
    <dif>
      Malnova <ref tip="lst" cel="unu.mezur0o" lst="voko:longoj">mezurunuo</ref> de
      longo a&ubreve; profundo, responda proksimume al la distanco inter la pintoj de
      la etenditaj brakoj de plenkreskulo,
      respektive 6&#x202F;-&#x202F;10 futoj (franca: 195&#x202F;cm; angla, germana: 183&#x202F;cm;
      rusa: 213&#x202F;cm):
      <ekz>
        171 <tld/>oj sub la supra&jcirc;o de la akvo
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili alproksimi&gcirc;as al ia lando kaj sondinte, ili trovis dudek <tld/>ojn
        <fnt>
          <bib>NT</bib>
          <lok>&Ago; 27:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        la boato forna&gcirc;is kelkajn <tld/>ojn for de la bordo
        <fnt><aut>I. G. &Scirc;irjaev</aut> <vrk>Sen titolo</vrk>,
        <lok>&ccirc;apitro 29a</lok></fnt>;
      </ekz>
      <ekz>
        nur unu <tld/>on sub la akvo staris la &scirc;iprompita&jcirc;o
        <fnt>
          <bib>Fab4</bib>
          <lok>Anneto</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu kirko de la Relevi&gcirc;o rondforma, simetria, <klr>[&#8230;]</klr> havas po 30
        <tld/>ojn la&ubreve;longe kaj la&ubreve;lar&gcirc;e
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 1 (63)</lok>
        </fnt>;
      </ekz>
      <ekz>
        la infero falis dek mil <tld/>ojn pli profunden
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Klafter</trd>,
      <trd>Faden <klr>(Tiefenma&szlig;)</klr></trd>
    </trdgrp>
    <trd lng="es">toesa</trd>
    <trdgrp lng="fr">
      <trd>toise<klr tip="ind"> (mesure de longueur)</klr></trd>,
      <trd>brasse<klr tip="ind"> (mesure de longueur)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&ouml;l<klr tip="amb">    (hosszm&eacute;rt&eacute;k)</klr></trd>,
      <trd>fon&aacute;l<klr tip="amb">  (m&eacute;lys&eacute;gm&eacute;rt&eacute;k)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_a;&c_zh;&c_je;&c_n;&c_mol;</trd>,
      <trd>&c_f;&c_a;&c_t;&c_o;&c_m;</trd>,
      <trd>&c_m;&c_o;&c_r;&c_s;&c_k;&c_a;&c_ja; &c_s;&c_a;&c_zh;&c_je;&c_n;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x358A; <pr>x&#xFA;n</pr></trd>,
    <trd>&#x82F1;&#x5BFB; <pr>Y&#x12B;ngx&#xFA;n</pr></trd>
  </trdgrp>
</snc>
