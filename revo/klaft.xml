<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: klaft.xml,v 1.17 2019/02/12 23:10:44 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>klaft</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="klaft.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="klaft.0o.MEZ">
    <dif>
      Malnova <ref tip="super" cel="unu.mezur0o.SCI">mezurunuo</ref> de 
      longo a&ubreve; profundo (franca: 195 cm; germana: 183 cm; 
      rusa: 213 cm):
      <ekz>
        la boato forna&gcirc;is kelkajn <tld/>ojn for de la bordo
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>, 
        <lok>&ccirc;apitro 29a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Klafter</trd>
    <trd lng="es">toesa</trd>
    <trd lng="fr">toise<klr tip="ind"> (mesure de longueur)</klr></trd>
    <trd lng="hu">&ouml;l<klr tip="amb">
    (hosszm&eacute;rt&eacute;k)</klr>
</trd>
    <trd lng="ru">&c_s;&c_a;&c_zh;&c_je;&c_n;&c_mol;</trd>
  </snc>
  <snc mrk="klaft.0o.MEZ_angla">
    <dif>
      Angla mezurunuo de longo (183 cm).
    </dif>
    <trd lng="de">Faden</trd>
    <trd lng="fr">brasse anglaise<klr tip="ind"> (mesure de 
    longueur)</klr></trd>
    <trd lng="hu">fon&aacute;l<klr tip="amb"> 
(m&eacute;lys&eacute;gm&eacute;rt&eacute;k)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_f;&c_a;&c_t;&c_o;&c_m;</trd>,
      <trd>
&c_m;&c_o;&c_r;&c_s;&c_k;&c_a;&c_ja; &c_s;&c_a;&c_zh;&c_je;&c_n;&c_mol;
</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_zh;&c_a;&c_n;&c_mol;</trd>
  <trd lng="cs">s&aacute;h</trd>
  <trd lng="en">fathom</trd>
  <trd lng="it">braccio<klr tip="amb"> (antica misura)</klr>
</trd>
  <trd lng="nl">vadem</trd>
  <trd lng="pl">s&aogonek;&zdot;e&nacute;</trd>     
  <trd lng="ro">st&acirc;njen</trd>
  <trd lng="sk">siaha</trd>
</drv>
</art>
