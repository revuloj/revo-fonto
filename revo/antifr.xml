<art mrk="$Id: antifr.xml,v 1.19 2023/10/12 18:05:09 revo Exp $">
<kap>
  <rad>antifraz</rad>/o
</kap>
<drv mrk="antifr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELE</uzo>
  <uzo tip="fak">LIN</uzo>
  <snc mrk="antifr.0o.stilfiguro">
    <dif>
      <ref tip="super" cel="figur.stil0o">Stilfiguro</ref>
      per kiu oni ironie uzas vorton a&ubreve; esprimon
      en senco kontra&ubreve;a al &gcirc;ia normala signifo:
      <ekz>
        <tld/>o estas ironia a&ubreve; e&ccirc; sarkasma <ref cel="euxfem.0o">e&ubreve;femismo</ref>
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;tradukisto, perfidisto&rightquot;: la bone konata itala diro esprimas per <tld/>o la
        utopian ideon de traduka&jcirc;o kiel perfekta duobla&jcirc;o de originalo
        <fnt>
          <bib>MD14</bib>
          <vrk>Literaturo &dash; Traduki &Scirc;ekspiron</vrk>
        </fnt>;
      </ekz>
      <ekz>
        nomata <tld/>e &leftquot;simpligita traktato&rightquot; <klr>[&#x2026;]</klr> la nova teksto
        <klr>[&#x2026;]</klr> enhavas ne malpli ol ducent kvindek ses pa&gcirc;ojn kun preska&ubreve;
        tricent modifoj
        <fnt>
          <bib>MD05</bib>
          <vrk>En Lisbono, en la Dorso de la Popoloj</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_t;&c_f;&c_r;&c_a;&c_z;&c_y;&c_s;</trd>
  <trd lng="ca">ant&iacute;frasi</trd>
  <trd lng="cs">antifr&aacute;ze</trd>
  <trd lng="de">Antiphrase</trd>
  <trd lng="en">antiphrase</trd>
  <trd lng="es">ant&iacute;frasis</trd>
  <trd lng="fa">&a_b;&f_y;&a_A;&a_n; &a_m;&a_ayn;&f_k;&a_w;&a_s;</trd>
  <trd lng="fr">antiphrase</trd>
  <trd lng="hu">antifr&aacute;zis</trd>
  <trdgrp lng="ja">
    <trd>&#x8A9E;&#x610F;&#x306E;&#x53CD;&#x7528;  <pr>&#x3054;&#x3044;&#x306E;&#x306F;&#x3093;&#x3088;&#x3046;</pr></trd>,
    <trd>&#x53CD;&#x8A9E;  <pr>&#x306F;&#x3093;&#x3054;</pr></trd>
  </trdgrp>
  <trd lng="pl">antyfraza</trd>
  <trd lng="ro">antifraza</trd>
  <trd lng="sk">&uacute;myseln&eacute; pou&zcaron;itie slova alebo v&yacute;razu v opa&ccaron;nom v&yacute;zname</trd>
  <trd lng="uk">&c_a;&#x43D;&c_t;&c_i;&c_f;&c_r;&c_a;&c_z;&c_i;&c_s;</trd>
</drv>
</art>
