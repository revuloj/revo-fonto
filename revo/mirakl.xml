<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>mirakl</rad>/o
</kap>
<drv mrk="mirakl.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Eksterordinara fenomeno kontra&ubreve;a al la le&gcirc;oj de naturo.
    </dif>
    <trdgrp lng="de">
      <trd>&uuml;bernat&uuml;rliches Ereignis</trd>,
      <trd>Wunder</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Mirindega farita&jcirc;o:
      <ekz>
        la <tld/>oj de la scienco.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Wunderwerk</trd>,
      <trd>Wunder</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_c;&c_u;&c_d;</trd>
  <trd lng="en">miracle</trd>
  <trdgrp lng="nl">
    <trd>wonder</trd>,
    <trd>mirakel</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>milagro</trd>
  </trdgrp>
  <trd lng="hu">csoda</trd>
  <trd lng="ru">&c_ch;&c_u;&c_d;&c_o;</trd>
  <trd lng="sk">z&aacute;zrak</trd>
</drv>
</art>
