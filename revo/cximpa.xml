<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cximpa.xml,v 1.38 2023/10/31 13:37:04 revo Exp $">
<kap>
  <ofc>9</ofc>
  <rad>&ccirc;impanz</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="cximpa.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="cximpa.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el la familio
      <ref tip="malprt" cel="homed.0oj">homedoj</ref>
      (<trd lng="la">Pan</trd>), de afrikaj homsimilaj 
      <ref tip="lst" cel="simi.0o" lst="voko:simioj">simioj</ref> kun du
      specioj:
      <ekz>
        ial hieno estas pli simila al lupo, anstata&ubreve; <tld/>o estas
        desegnita paviano, kaj anstata&ubreve; pantero &ndash; tigro
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="&Monato;/2007/009009.php">Ruza
          leporo kaj &ccirc;iuj aliaj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e kelkaj specimenoj de homsimilaj simioj, la <tld/>oj, estis
        trovitaj virusoj gene proksimaj al la plej disvasti&gcirc;inta viruso
        (HIV-1)
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2004/008160.html">Afriko kaj
          aidoso</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>ino scipovas elmontri &ccirc;iujn siajn dezirojn: man&gcirc;i,
        trinki, uzi necesejon
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2004/008297.html">Havi
          &ccirc;impanzon hejme</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj kisas, kvankam <klr>[&#x2026;]</klr> ne tiom ofte kiom homoj
        <fnt>
          <bib>Monato</bib>
          <aut>Daniel Grassam</aut>
          <vrk><url ref="&Monato;/2014/011181.php">Kisojn
          el Oksfordo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x9ED1;&#x7329;&#x7329;&#x5C5E; <pr>h&#x113;ix&#x12B;ngxingsh&#x1D4;</pr></trd>
</snc>
  <snc mrk="cximpa.0o.specio">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      La pli granda <ref tip="lst" cel="speci.0o.BIO" lst="voko:zoologiaj_specioj">specio</ref> el
      tiu genro
      (<trd lng="la">Pan troglodytes</trd>).
    </dif>
    <trd lng="zh">&#x9ED1;&#x7329;&#x7329; <pr>h&#x113;ix&#x12B;ngxing</pr></trd>
</snc>
  <trd lng="be">&c_sh;&c_y;&c_m;&c_p;&c_a;&c_n;&c_z;&c_e;</trd>
  <trd lng="bg">&c_sh;&c_i;&c_m;&c_p;&c_a;&c_n;&c_z;&c_je;</trd>
  <trd lng="de">Schimpanse</trd>
  <trd lng="el">&chi;&jota;&my;&pi;&alfa;&ny;&tau;&zeta;&eta_ton;&sigma_fina;</trd>
  <trd lng="en">chimpanzee</trd>
  <trd lng="es">chimpanc&eacute;</trd>
  <trdgrp lng="fa">
    <trd>&a_sh;&a_m;&f_p;&a_A;&a_n;&a_z;&a_h;</trd>,
    <trd>&a_sh;&a_A;&a_m;&f_p;&a_A;&a_n;&a_z;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">chimpanz&eacute;</trd>
  <trd lng="he">&#x5E9;&#x5C1;&#x5B4;&#x5D9;&#x5DE;&#x5E4;&#x5BC;&#x5B7;&#x5E0;&#x5D6;&#x5B8;&#x5D4;</trd>
  <trd lng="hu">csimp&aacute;nz</trd>
  <trd lng="id">simpanse</trd>
  <trd lng="it">scimpanz&eacute;</trd>
  <trd lng="ja">&#x30C1;&#x30F3;&#x30D1;&#x30F3;&#x30B8;&#x30FC;</trd>
  <trd lng="nl">chimpansee</trd>
  <trd lng="pt">chimpanz&eacute;</trd>
  <trd lng="pl">szympans</trd>
  <trd lng="ru">&c_sh;&c_i;&c_m;&c_p;&c_a;&c_n;&c_z;&c_je;</trd>
  <trd lng="sk">&scaron;impanz</trd>
  <trd lng="sv">schimpans</trd>
  <trd lng="uk">&c_sh;&c_i;&#x43C;&#x43F;&c_a;&#x43D;&c_z;&c_je;</trd>
</drv>

<drv mrk="cximpa.malgranda0o">
  <kap>malgranda <tld/>o</kap>
  <snc mrk="cximpa.malgranda0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      La pli malgranda <ref tip="lst" cel="speci.0o.BIO" lst="voko:zoologiaj_specioj">specio</ref>
      el tiu genro
      (<trd lng="la">Pan paniscus</trd>).
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_n;&c_o;&c_b;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_l;&c_ib;&c_k;&c_a;&c_v;&c_y; <ind>&c_sh;&c_y;&c_m;&c_p;&c_a;&c_n;&c_z;&c_e;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Zwergschimpanse</trd>,
    <trd>Bonobo</trd>
  </trdgrp>
  <trd lng="bg">&c_b;&c_o;&c_n;&c_o;&c_b;&c_o;</trd>
  <trd lng="en">dwarf <ind>chimpanzee</ind></trd>
  <trdgrp lng="fr">
    <trd>chimpanz&eacute; nain</trd>
    <trd>bonobo</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>t&ouml;rpecsimp&aacute;nz</trd>,
    <trd>bonob&oacute;</trd>
  </trdgrp>
  <trd lng="id">bonobo</trd>
  <trd lng="it">bonobo</trd>
  <trdgrp lng="nl">
    <trd>dwergchimpansee</trd>,
    <trd>bonobo</trd>
  </trdgrp>
  <trd lng="pl">szympans kar&lstroke;owaty</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_r;&c_l;&c_i;&c_k;&c_o;&c_v;&c_y;&c_j;
    &c_sh;&c_i;&c_m;&c_p;&c_a;&c_n;&c_z;&c_je;</trd>,
    <trd>&c_b;&c_o;&c_n;&c_o;&c_b;&c_o;</trd>
  </trdgrp>
  <trd lng="zh">&#x502D;&#x9ED1;&#x7329;&#x7329; <pr>w&#x14D;h&#x113;ix&#x12B;ngxing</pr></trd>
</drv>
</art>
