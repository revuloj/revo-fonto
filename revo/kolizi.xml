<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kolizi.xml,v 1.24 2025/11/15 14:28:14 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>kolizi</rad>/i
<fnt><bib>JED</bib></fnt></kap>
<drv mrk="kolizi.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kolizi.0i.kunpusxigxi">
    <dif>
      Forte <ref tip="super" cel="pusx.kun0igxi">kunpu&scirc;i&gcirc;i</ref>:
      <ekz>
        la ekspresa vagonaro <tld/>is kun vara trajno;
      </ekz>
      <ekz>
        kamiono <tld/>is kontra&ubreve; aro da rubujoj
        <fnt>
          <bib>Kon11</bib>
          <lok>Blogo pri</lok>
        </fnt>;
      </ekz>
      <ekz>
        aviadilo <tld/>is kun monto
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2003/007928.html"
              >E&ccirc; se vi ne estas aviadisto ...</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>as &scirc;ipo kun la &scirc;ipego <klr>[&#x2026;]</klr> kaj mortas 844 homoj
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        elirante tiun matenon, &scirc;i preska&ubreve; <tld/>is kun <nom>Marta</nom>
        <fnt>
          <bib>CKv</bib>
          <lok>17</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia dorso <tld/>is kun la korpo de la malgranda <nom>Klem</nom> kaj tuj forigis lian
        ekvilibron
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 5</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="karamb.0i.FIG">karamboli</ref>
    </refgrp>
    <ref tip="vid" cel="pusx.inter0igxo">interpu&scirc;i&gcirc;o</ref>
    <trdgrp lng="be">
      <trd>&c_s;&c_u;&c_t;&c_y;&c_k;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_s;&c_a;&c_p;&c_h;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_u;&c_r;&c_e;&c_z;&c_a;&c_c;&c_c;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>zusammensto&szlig;en</trd>,
      <trd>kollidieren</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>collide with</trd>,
      <trd>crash into</trd>,
      <trd>run into</trd>,
    </trdgrp>
    <trd lng="pl">zderza&cacute; si&eogonek;</trd>
  <trdgrp lng="pt">
    <trd>colidir</trd>,
    <trd>ir de <ind>encontro</ind> a</trd>,
    <trd>chocar-se</trd>,
    <trd>abalroar</trd>
  </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_t;&c_o;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_v;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kolizi.0i.ekbatali">
    <dif>
      Komenci <ref tip="super" cel="batal.inter0i">interbatali</ref>:
      <ekz>
        en 1753 la brita ekspedicia trupo tie, sub la komando de George Washington,
        <tld/>is kun franca trupo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=798999"
              >Rilatoj inter Francio kaj Unui&gcirc;inta Re&gcirc;lando</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_u;&c_t;&c_y;&c_k;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_u;&c_v;&c_a;&c_j;&c_s;&c_mol;&c_c;&c_ib; &c_w; <ind>&c_k;&c_a;&c_n;&c_f;&c_l;&c_ib;&c_k;&c_t;</ind></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>angreifen</trd>,
      <trd>treffen <klr>(auf, Milit.)</klr></trd>,
      <trd>aufeinander treffen <klr>(Truppen)</klr></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>engage</trd>,
      <trd>clash</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_t;&c_o;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_v;&c_s;&c_t;&c_u;&c_p;&c_i;&c_t;&c_mol; &c_v; &c_k;&c_o;&c_n;&c_f;&c_l;&c_i;&c_k;&c_t;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kolizi.0i.konflikti">
    <uzo tip="stl">FIG</uzo>
    <dif>
      (pp cirkonstancoj a&ubreve; sentoj)
      Neakordi&gcirc;i, <ref tip="super" cel="kontraux.0i">kontra&ubreve;i</ref>, <ref tip="super" cel="konfli.0i">konflikti</ref>:
      <ekz>
        tia entrepreno <tld/>us kun la le&gcirc;o;
      </ekz>
      <ekz>
        individueca sinteno <tld/>is kun la demokrata spirito
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>is la kontra&ubreve;aj tendencoj, kiam ok landaj asocioj prezentis projekton de
        rezolucio al la Kongreso
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        solvoj tiaj, tamen, <tld/>as kun manko de politika volo inter la reprezentantoj
        de nuna kapitalismo
        <fnt>
          <bib>Monato</bib>
          <aut>Josef Mendl</aut>
          <vrk><url ref="&Monato;/2005/008701.php"
              >Terorismo: koncentri&gcirc;u pri la ka&ubreve;zoj</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[se]</klr> iuj paketoj <tld/>is kun la paketoj de tute alia komunikado, la
        ricevilo povas baze de la kunsenditaj paketnumeroj reordigi la tuton
        <fnt>
          <bib>Monato</bib>
          <aut>Pejno Simono</aut>
          <vrk>Prefere pakete</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_u;&c_v;&c_a;&c_j;&c_s;&c_mol;&c_c;&c_ib; &c_w; <ind>&c_k;&c_a;&c_n;&c_f;&c_l;&c_ib;&c_k;&c_t;</ind></trd>,
      <trd>&c_k;&c_a;&c_n;&c_f;&c_l;&c_ib;&c_k;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_u;&c_p;&c_ja;&c_r;&c_e;&c_ch;&c_y;&c_c;&c_mol;</trd>,
      <trd>&c_u;&c_v;&c_a;&c_j;&c_s;&c_mol;&c_c;&c_ib; &c_w; <ind>&c_s;&c_u;&c_p;&c_ja;&c_r;&c_e;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</ind></trd>,
    </trdgrp>
    <trdgrp lng="de">
      <trd>in Konflikt geraten</trd>,
      <trd>versto&szlig;en (gegen)</trd>,
      <trd>kollidieren</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_n;&c_f;&c_l;&c_i;&c_k;&c_t;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_s;&c_t;&c_u;&c_p;&c_i;&c_t;&c_mol; &c_v; &c_k;&c_o;&c_n;&c_f;&c_l;&c_i;&c_k;&c_t;</trd>
    </trdgrp>
  </snc>
  <trd lng="bo">&t_ra;&t_data;&t_ba;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>kolidovat</trd>,
    <trd>srazit se</trd>
  </trdgrp>
  <trd lng="es">colisionar</trd>
  <trdgrp lng="fr">
    <trd>entrer en <ind>collision</ind></trd>
    <trd>se <ind>caramboler</ind></trd>,
    <trd>emboutir<klr tip="ind"> (entrer en collision)</klr></trd>,
    <trd>heurter</trd>,
    <trd>se <ind>heurter</ind> (&agrave;)</trd>,
    <trd>percuter<klr tip="ind"> (heurter)</klr></trd>,
    <trd>rentrer dans<klr tip="ind"> (entrer en collision
    avec)</klr></trd>,
    <trd>tamponner<klr tip="ind"> (heurter)</klr></trd>,
    <trd>t&eacute;lescoper<klr tip="ind"> (heurter)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x885D;&#x7A81;&#x3059;&#x308B;  <pr>&#x3057;&#x3087;&#x3046;&#x3068;&#x3064;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x3076;&#x3064;&#x304B;&#x308B;</trd>
  </trdgrp>
  <trd lng="nl">botsen</trd>
  <trdgrp lng="sk">
    <trd>kolidova&tcaron;</trd>,
    <trd>nezhodova&tcaron; sa</trd>,
    <trd>zrazi&tcaron; sa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_sh;&c_t;&c_o;&c_v;&c_h;&c_n;&c_u;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_ib;&c_t;&c_k;&c_n;&c_u;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_s;&c_t;&c_u;&c_p;&c_i;&c_t;&c_i; &c_v; &c_k;&c_o;&c_n;&c_f;&c_l;&c_ib;&c_k;&c_t;</trd>,
    <trd>&c_v;&c_r;&c_ib;&c_z;&c_a;&c_t;&c_i;&c_s;&c_ja; (&c_p;&c_r;&c_o; &c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;&c_ib;&c_l;&c_mol; &c_ib; &c_t;.&c_p;.)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x649E; <pr>zhu&agrave;ng</pr></trd>,
    <trd>&#x76F8;&#x649E; <pr>xi&amacron;ngzhu&agrave;ng</pr></trd>,
    <trd>&#x78B0;&#x649E; <pr>p&egrave;ngzhu&agrave;ng</pr></trd>,
    <trd>&#x51B2;&#x649E; <pr>ch&omacron;ngzhu&agrave;ng</pr></trd>,
    <trd>&#x649E;&#x8ECA; <pr>zhu&agrave;ngch&emacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kolizi.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        apena&ubreve; &scirc;i eliris el la a&ubreve;tovojego, jam okazis tiu <tld/>o, kaj policisto tuj
        venis
        <fnt>
          <bib>ChR</bib>
          <lok>Tri estas tro</lok>
        </fnt>;
      </ekz>
      <ekz>
        se trafiksistemoj kolapsos, <tld/>oj de a&ubreve;toj, tramoj kaj fervojoj estas
        programitaj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan MAUL</aut>
          <vrk>Militado sen bataliloj</vrk>
        </fnt>;
      </ekz>
      <ekz>
        komuniki&gcirc;adi inter si pace kaj frate, sen reciprokaj <tld/>oj, malamo kaj
        maljusteco
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Dogmoj de Hilelismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        oni &scirc;ajnis rimarki seriozi&gcirc;antan interkulturan <tld/>on
        <fnt>
          <bib>Kon11</bib>
          <lok>E&ccirc; se &ccirc;iuj devenis el diversaj landoj...</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vortoj kaj formoj ne venu en <tld/>on unu kun alia
        <fnt>
          <bib>LAE</bib>
          <lok>5. Leksiko kaj Semantiko</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_t;&c_y;&c_k;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_ib;&c_z;&c_ib;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_f;&c_l;&c_ib;&c_k;&c_t;</trd>,
    <trd>&c_s;&c_u;&c_p;&c_ja;&c_r;&c_e;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kolize</trd>,
    <trd>rozpor</trd>,
    <trd>sr&aacute;&zcaron;ka</trd>,
    <trd>st&rcaron;et</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Zusammensto&szlig;</trd>,
    <trd>Kollision</trd>
  </trdgrp>
  <trd lng="en">collision</trd>
  <trd lng="es">colisi&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>collision</trd>,
    <trd>antagonisme<klr tip="ind"> (heurt)</klr></trd>
    <trd>carambolage<klr tip="ind"> (heurt)</klr></trd>,
    <trd>choc<klr tip="ind"> (heurt)</klr></trd>,
    <trd>conflit<klr tip="ind"> (heurt)</klr></trd>,
    <trd>heurt</trd>,
    <trd>opposition<klr tip="ind"> (heurt)</klr></trd>,
    <trd>tamponnement<klr tip="ind"> (heurt)</klr></trd>,
    <trd>t&eacute;lescopage</trd>
  </trdgrp>
  <trd lng="ja">&#x885D;&#x7A81;  <pr>&#x3057;&#x3087;&#x3046;&#x3068;&#x3064;</pr></trd>
  <trd lng="nl">botsing</trd>
  <trd lng="pl">kolizja</trd>
  <trd lng="ro">coliziune</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_o;&c_l;&c_k;&c_n;&c_o;&c_v;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_l;&c_i;&c_z;&c_i;&c_ja;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_f;&c_l;&c_i;&c_k;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kol&iacute;zia</trd>,
    <trd>zr&aacute;&zcaron;ka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_k;&c_o;&c_l;&c_ib;&c_z;&c_ib;&c_ja;</trd>,
    <trd>&c_z;&c_ib;&c_t;&c_k;&c_n;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_f;&c_l;&c_ib;&c_k;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x78B0;&#x649E; <pr>p&egrave;ngzhu&agrave;ng</pr></trd>,
    <trd>&#x76F8;&#x649E; <pr>xi&amacron;ngzhu&agrave;ng</pr></trd>,
    <trd>&#x51B2;&#x7A81; <pr>ch&omacron;ngt&umacron;</pr></trd>
  </trdgrp>
</drv>
</art>
