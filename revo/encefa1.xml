<kap>
  <rad>encefalit</rad>/o
</kap>
<drv mrk="encefa1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="inflam.0o" lst="voko:malsanoj">Inflamo</ref>
      de la encefalo:
      <ekz>
        la virusa ka&ubreve;zo de diversaj <tld/>oj estas bone
        konata<fnt><aut>Vladimir Lemelev</aut>: <vrk>Mikroboj provokas
        neinfektajn malsanojn &scirc;an&gcirc;i&gcirc;i al
        infektaj</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 1a, p.
        16-17a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_n;&c_c;&c_e;&c_f;&c_a;&c_l;&c_ib;&c_t;</trd>
  <trd lng="ca">encefalitis</trd>
  <trdgrp lng="cs">
    <trd>encefalitida</trd>,
    <trd>z&#xE1;n&#x11B;t mozku</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gehirnentz&uuml;ndung</trd>,
    <trd>Hirnhautentz&uuml;ndung</trd>,
    <trd>Enzephalitis<klr> [fak]</klr></trd>
  </trdgrp>
  <trd lng="es">encefalitis</trd>
  <trd lng="fr">enc&eacute;phalite</trd>
  <trdgrp lng="hu">
    <trd>agyvel&odblac;gyullad&aacute;s</trd>,
    <trd>encephalitis</trd>
  </trdgrp>
  <trd lng="id">ensefalitis</trd>
  <trd lng="ja">&#x8133;&#x708E;  <pr>&#x306E;&#x3046;&#x3048;&#x3093;</pr></trd>
  <trdgrp lng="nl">
    <trd>encefalitis</trd>,
    <trd>hersenontsteking</trd>
  </trdgrp>
  <trd lng="pl">zapalenie <ind>m&oacute;zgu</ind></trd>
  <trd lng="pt">encefalite</trd>
  <trd lng="ru">&c_e;&c_n;&c_c;&c_je;&c_f;&c_a;&c_l;&c_i;&c_t;</trd>
  <trd lng="sk">z&#xE1;pal mozgu</trd>
  <trdgrp lng="uk">
    <trd>&#x435;&#x43D;&#x446;&#x435;&#x444;&#x430;&#x43B;&#x456;&#x442;</trd>,
    <trd>&#x437;&#x430;&#x43F;&#x430;&#x43B;&#x435;&#x43D;&#x43D;&#x44F; &#x433;&#x43E;&#x43B;&#x43E;&#x432;&#x43D;&#x43E;&#x433;&#x43E; &#x43C;&#x43E;&#x437;&#x43A;&#x443;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8111;&#x708E; <pr>n&#x1CE;oy&#xE1;n</pr></trd>,
    <trd>&#x8111;&#x819C;&#x708E; <pr>n&#x1CE;om&#xF2;y&#xE1;n</pr></trd>
  </trdgrp>
</drv>
</art>
