<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>cikut</rad>/o
</kap>
<drv mrk="cikut.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="api.0acoj">apiacoj</ref>
      (<trd lng="la">Cicuta</trd>), tre venena, simila al
      <ref tip="vid" cel="koni.0o">konio</ref>:
      <ekz>
        salatoj el semoj de fungoj, malsekaj bu&scirc;oj de musoj kaj cikuto
        <fnt>(<aut>Andersen, trad. Zamenhof</aut>: <vrk>Fabeloj,
        Monteto de elfoj</vrk>)</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Venena suko el tiu vegeta&jcirc;o:
      <ekz>
        la Atenanoj kondamnis Sokraton trinki <tld/>on.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Wasserschierling</trd>
  <trd lng="ru">&c_c;&c_i;&c_k;&c_u;&c_t;&c_a;</trd>
</drv>
</art>
