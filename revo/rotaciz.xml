   <kap>
    <rad>rotaciz</rad>/o <fnt><bib>PIV1</bib></fnt>
   </kap>
   <drv mrk="rotaciz.0o">
    <kap><tld/>o</kap>
    <snc>
    <uzo tip="fak">MED</uzo>
    <uzo tip="fak">FON</uzo>
     <dif>
      Nepovo prononci la sonon R:
      <ekz>anstata&ubreve; &#8218;vero&#8216; li &ccirc;iam prononcas &#8218;velo&#8216;. </ekz>
     </dif>
      <ref tip="vid" cel="lambdaciz.0o">lambdacizo</ref>
    </snc>
    <trd lng="en">rhotacism</trd>
    <trd lng="be">&c_r;&c_a;&c_t;&c_a;&c_c;&c_y;&c_z;&c_m;</trd>
    <trd lng="de">Rhotazismus</trd>
    <trd lng="fr">rhotacisme</trd>
    <trd lng="hu">rotacizmus</trd>
    <trdgrp lng="pl">
      <trd>reranie</trd>,
      <trd>rotacyzm</trd>
    </trdgrp>
    <trd lng="pt">rotacismo</trd>
   </drv>
  </art>


