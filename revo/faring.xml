<art mrk="$Id: faring.xml,v 1.23 2023/08/31 13:11:27 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>faring</rad>/o
</kap>
<drv mrk="faring.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Kava parto inter la bu&scirc;o kaj la ezofaga tubo:  
      <ekz>
        &ccirc;e aidoso-malsanuloj herpeto konstante recidivas kaj ofte atakas <tld/>on, ezofagon, <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2007/008991.php">La &ccirc;efaj murdistoj de HIV-infektitoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        tipa, tre turmenta, simptomo: akvotimego, <klr>[&#x2026;]</klr> dum iu provo 
        eltrinki e&ccirc; unu guton de akvo, aperas konvulsiaj kunstre&ccirc;igoj de <tld/>omuskoloj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2008/009550.php">Batalo
           kontra&ubreve; mortiga malsano</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fauxk.0o.ZOO">fa&ubreve;ko<sncref/></ref>,
      <ref cel="gorgx.0o">gor&gcirc;o</ref>,
      <ref cel="glut.0ejo.ANA">glutejo<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_l;&c_o;&c_t;&c_k;&c_a;</trd>
  <trd lng="ca">faringe</trd>
  <trdgrp lng="cs">
    <trd>farynx</trd>,
    <trd>hltan</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Schlundkopf</trd>,
     <trd>Rachen</trd>,
     <trd>Pharynx</trd>
  </trdgrp>
  <trd lng="en">pharynx</trd>
  <trd lng="es">faringe</trd>
  <trd lng="fr">pharynx</trd>
  <trd lng="hu">garat</trd>
  <trdgrp lng="id">
    <trd>esofagus</trd>,
    <trd>kerongkongan</trd>
  </trdgrp>
  <trd lng="it">faringe</trd>
  <trd lng="ja">&#x54BD;&#x982D;  [<ind>&#x3044;&#x3093;&#x3068;&#x3046;</ind>]</trd>
  <trdgrp lng="nl">
   <trd>keelholte</trd>,
   <trd>farynx</trd>
  </trdgrp>
  <trd lng="pl">gardziel</trd>
  <trd lng="pt">faringe</trd>
  <trd lng="ro">g&acirc;t</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_l;&c_o;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_je;&c_v;</trd>
  </trdgrp>
  <trd lng="sk">hltan</trd>
  <trdgrp lng="uk">
    <trd>&#x433;&#x43B;&#x43E;&#x442;&#x43A;&#x430;</trd>,
    <trd>&#x437;&#x456;&#x432;</trd>
  </trdgrp>
</drv>
</art>
