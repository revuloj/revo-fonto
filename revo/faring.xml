  <trdgrp lng="nl">
   <trd>keelholte</trd>,
   <trd>farynx</trd>
</trdgrp>
<trdgrp lng="ru">
    <trd>&c_g;&c_l;&c_o;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_je;&c_v;</trd>
  </trdgrp>
