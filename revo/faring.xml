<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>faring</rad>/o
</kap>
<drv mrk="faring.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Kava parto inter la bu&scirc;o kaj la ezofaga tubo.  
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_l;&c_o;&c_t;&c_k;&c_a;</trd>
  <trdgrp lng="de">
     <trd>Schlundkopf</trd>,
     <trd>Rachen</trd>,
     <trd>Pharynx</trd></trdgrp>
  <trd lng="en">pharynx</trd>
  <trd lng="fr">pharynx</trd>
  <trd lng="hu">garat</trd>
  <trdgrp lng="nl">
   <trd>keelholte</trd>,
   <trd>farynx</trd>
</trdgrp>
  <trd lng="pl">gardziel</trd>
<trdgrp lng="ru">
    <trd>&c_g;&c_l;&c_o;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_je;&c_v;</trd>
  </trdgrp>
</drv>
</art>
