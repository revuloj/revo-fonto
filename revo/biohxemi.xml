<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: biohxemi.xml,v 1.14 2022/06/03 07:28:22 revo Exp $">
<kap>
  <rad>bio&hcirc;emi</rad>/o <fnt><bib>PIV1</bib></fnt>,
  <var><kap><rad var="k">biokemi</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>

<drv mrk="biohxemi.0o">
  <kap><tld/>o,
    <var><kap><tld var="k"/>o</kap></var>
  </kap>
  <snc mrk="biohxemi.0o.SCI">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Biologia <ref tip="lst" lst="voko:sciencoj" cel="hxemi.0o">&hcirc;emio</ref>,
      studo de la &hcirc;emiaj substancoj kaj procezoj en la viva&jcirc;oj:
      <ekz>
        en 1907 <nom>Buchner</nom> ricevis la nobel-premion pri &hcirc;emio pro siaj <tld/>aj esploroj
        kaj la malkovro de fermentado sendependa de &ccirc;elo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1148">Enzimo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Asimov</nom> laboris kiel profesoro de <tld/>o
        <fnt>
          <bib>Viki</bib>,
          <vrk><url ref="&Viki;/Isaac_Asimov">
          Isaac Asimov</url></vrk>,
          <lok>2010-08-09</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fajrobrigado el la apuda urbeto decidis anka&ubreve; voki specialistojn pri <tld var="k"/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Helga Rapley</aut>
          <vrk><url ref="&Monato;/2004/008356.html">Blanka dramo en Bureso</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        evolua <tld var="k"/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="&Monato;/2009/009858.php">La mondrevolucia verko finfine en
          Esperanto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ib;&c_ja;&c_h;&c_ib;&c_m;&c_ib;&c_ja;</trd>
  <trd lng="ca">bioqu&iacute;mica</trd>
  <trd lng="de">Biochemie</trd>
  <trd lng="en">biochemistry</trd>
  <trd lng="es">bioqu&iacute;mica</trd>
  <trd lng="fr">biochimie</trd>
  <trd lng="he">&bet;&yod;&vav;&kaf;&yod;&mem;&yod;&he;</trd>
  <trd lng="nl">biochemie</trd>
  <trd lng="pl">biochemia</trd>
  <trd lng="ru">&c_b;&c_i;&c_o;&c_h;&c_i;&c_m;&c_i;&c_ja;</trd>
  <trd lng="uk">&c_b;&c_ib;&#x43E;&c_h;&c_ib;&#x43C;&c_ib;&#x44F;</trd>
</drv>

<drv mrk="biohxemi.0isto">
  <kap><tld/>isto,
    <var><kap><tld var="k"/>isto</kap></var>
  </kap>
  <snc mrk="biohxemi.0isto.SCI">
    <dif>
      Fakulo pri <tld/>o:
      <ekz>
        <nom>Konstantin Efetov</nom> <klr> [&#8230;]</klr> estas ukrainia sciencisto (biologo kaj
        <tld/>isto)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=291072">Konstantin Efetov</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Erwin Chargaff</nom> <klr> [&#8230;]</klr> estis a&ubreve;stra <tld var="k"/>isto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=241904">Erwin Chargaff</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ib;&c_ja;&c_h;&c_ib;&c_m;&c_ib;&c_k;</trd>
  <trd lng="ca">bioqu&iacute;mic</trd>
  <trd lng="de">Biochemiker</trd>
  <trd lng="en">biochemist</trd>
  <trd lng="es">bioqu&iacute;mico</trd>
  <trd lng="fr">biochimiste</trd>
  <trd lng="he">&bet;&yod;&vav;&kaf;&yod;&mem;&alef;&yod;</trd>
  <trd lng="nl">biochemicus</trd>
  <trd lng="pl">biochemik</trd>
  <trd lng="ru">&c_b;&c_i;&c_o;&c_h;&c_i;&c_m;&c_i;&c_k;</trd>
</drv>
</art>
