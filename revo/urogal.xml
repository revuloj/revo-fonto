<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>urogal</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="urogal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="urogal.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Specio de <ref tip="super" cel="tetraon.0o">tetraono</ref>
      (<trd lng="la">Tetrao urogallus</trd>):
      <ekz>
        la <tld/>o <klr>...</klr> estas fama birdo pro sia ceremonia 
        pari&gcirc;ado
        <fnt>
          <aut>J. Hauser</aut>,
          <vrk>&Ccirc;u minacata specio?</vrk>,
          <bib>Monato</bib>,
          <lok>2017:08-09</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_l;&c_u;&c_sh;&c_e;&c_c;</trd>
  <trd lng="ca"><ind>gall</ind> salvatge</trd>
  <trdgrp lng="cs">
    <trd>tet&rcaron;ev hlu&scaron;ec</trd>,
    <trd>tet&rcaron;ev hlu&scaron;ec</trd>
  </trdgrp>
  <trd lng="de">Auerhuhn</trd>
  <trd lng="en">capercaillie</trd>
  <trd lng="es">urogallo</trd>
  <trd lng="fi">metso</trd>
  <trd lng="fr">grand coq de bruy&egrave;re</trd>
  <trd lng="hu">siketfajd</trd>
  <trdgrp lng="it">
    <trd>gallo<klr> cedrone</klr></trd>,
    <trd>urogallo</trd>
   </trdgrp>
  <trd lng="nl">auerhoen</trd>
  <trd lng="pl">g&lstroke;uszec</trd>
  <trdgrp lng="pt">
    <trd>tetraz</trd>,
    <trd>galo-da-floresta</trd>,
    <trd>tetraz-grande-das-serras</trd>
   </trdgrp>
  <trd lng="ru">&c_g;&c_l;&c_u;&c_h;&c_a;&c_r;&c_mol;</trd>
  <trd lng="sk">tetrov</trd>
  <trd lng="sv">tj&auml;der</trd>
</drv>
</art>
