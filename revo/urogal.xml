<vortaro>
<art mrk="$Id: urogal.xml,v 1.21 2023/09/05 09:43:53 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>urogal</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="urogal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="urogal.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Specio de <ref tip="super" cel="tetraon.0o">tetraono</ref>
      (<trd lng="la">Tetrao urogallus</trd>):
      <ekz>
        la <tld/>o <klr>[&#x2026;]</klr> estas fama birdo pro sia ceremonia 
        pari&gcirc;ado
        <fnt>
          <aut>J. Hauser</aut>,
          <vrk>&Ccirc;u minacata specio?</vrk>,
          <bib>Monato</bib>,
          <lok>2017:08-09</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj malofti&gcirc;is pro tio, ke &ccirc;iam pli rare ili trovas ta&ubreve;gajn lokojn por siaj printempaj seksludoj
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2013/011065.php">&Ccirc;asado</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/1/1c/Tetrao_urogallus_%28cropped%29.jpg/618px-Tetrao_urogallus_%28cropped%29.jpg"><tld/>o dum pari&gcirc;a ceremonio (Tetrao urogallus), Nigra Arbaro, Germanujo
        <fnt>
          <aut>David Palmer, </aut>
          <lok><url ref="&WCW;/File:Tetrao_urogallus_(cropped).jpg">&WCW;/File:Tetrao_urogallus_(cropped).jpg</url></lok>
          <vrk><url ref="https://www.flickr.com/photos/22207425@N05/7169888957/">foto el Flickr</url></vrk>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_g;&c_l;&c_u;&c_sh;&c_e;&c_c;</trd>
  <trd lng="ca"><ind>gall</ind> salvatge</trd>
  <trdgrp lng="cs">
    <trd>tet&rcaron;ev hlu&scaron;ec</trd>,
    <trd>tet&rcaron;ev hlu&scaron;ec</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Auerhuhn</trd>,
    <trd>Auerhahn</trd>
  </trdgrp>
  <trd lng="en">capercaillie</trd>
  <trd lng="es">urogallo</trd>
  <trd lng="fi">metso</trd>
  <trd lng="fr">grand coq de bruy&egrave;re</trd>
  <trd lng="hu">siketfajd</trd>
  <trdgrp lng="it">
    <trd>gallo<klr> cedrone</klr></trd>,
    <trd>urogallo</trd>
   </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30E8;&#x30FC;&#x30ED;&#x30C3;&#x30D1;&#x30AA;&#x30AA;&#x30E9;&#x30A4;&#x30C1;&#x30E7;&#x30A6;</trd>,
    <trd>&#x30AA;&#x30AA;&#x30E9;&#x30A4;&#x30C1;&#x30E7;&#x30A6;</trd>
  </trdgrp>
  <trd lng="nl">auerhoen</trd>
  <trd lng="pl">g&lstroke;uszec</trd>
  <trdgrp lng="pt">
    <trd>tetraz</trd>,
    <trd>galo-da-floresta</trd>,
    <trd>tetraz-grande-das-serras</trd>
   </trdgrp>
  <trd lng="ru">&c_g;&c_l;&c_u;&c_h;&c_a;&c_r;&c_mol;</trd>
  <trd lng="sk">tetrov</trd>
  <trd lng="sv">tj&auml;der</trd>
  <trd lng="uk">&#x433;&#x43B;&#x443;&#x445;&#x430;&#x440;</trd>
</drv>
</art>
