<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nenial.xml,v 1.25 2024/09/25 15:32:33 revo Exp $">
<kap>
  <ofc>*</ofc><ofc>7</ofc>
  <rad>nenial</rad> <fnt> <bib>JED</bib></fnt>
</kap>
<drv mrk="nenial.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Adv. signifanta &leftquot;pro neniu ajn ka&ubreve;zo, motivo&rightquot;:
      <ekz>
        aldonante la prefikson &leftquot;nen&#x201D;, ni ricevas vortojn neajn: nenia, <tld/>,
        neniam, nenie, neniel, nenies, nenio, neniom, neniu
        <fnt><bib>F</bib>, <lok>&FE; 30</lok></fnt>.
      </ekz>
      <ekz>
        <tld/> vi koleras;
      </ekz>
      <ekz>
        tial, &ccirc;ar la sklavo deziris iri, mi <tld/> diris ne
        <fnt>
          <bib>Rob</bib>
          <lok>Parto I</lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;<klr>[&#8230;]</klr> plej postulende,&rightquot; mi diris, &leftquot;por ke la lo&gcirc;antoj en via plej
        perfekta civito <tld/> forlasu geometrion&rightquot;
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Sep</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i neniam kaj <tld/> plendu, &ccirc;ar oni disponigis al &scirc;i terasan apartamenton kun
        spirhaltiga panoramo
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2011/010183.php"
              >Terasa apartamento</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni <tld/> volas partopreni tiun agitadon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="baz.sen0a">senbaze</ref>,
      <ref cel="kauxz.sen0e">senka&ubreve;ze</ref>,
      <ref cel="kial.sen0a">senkiale</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_je;&c_s;&c_p;&c_r;&c_y;&c_ch;&c_y;&c_n;&c_n;&c_a;</trd>,
    <trd>&c_b;&c_je;&c_z; &c_d;&c_a;&c_j; &c_p;&c_r;&c_y;&c_ch;&c_y;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bez p&rcaron;&iacute;&ccaron;iny</trd>,
    <trd>pro nic za nic</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>grundlos</trd>,
     <trd>aus keinem <ind>Grund</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>for no <ind>reason (cause, motive)</ind></trd>,
    <trd>not for any <ind>reason (cause, motive)</ind></trd>,
    <trd>on no <ind>account</ind></trd>,
    <trd>without any <ind>reason (cause, motive)</ind> whatever</trd>
  </trdgrp>
  <trd lng="es">por ninguna raz&oacute;n</trd>
  <trd lng="fr">pour aucune cause</trd>
  <trdgrp lng="hu">
    <trd>semmi&eacute;rt<klr tip="ind"> (semmilyen okb&oacute;l)</klr></trd>,
    <trd>semmilyen okn&aacute;n&aacute;l fogva</trd>
  </trdgrp>
  <trd lng="ja">&#12393;&#12435;&#x306A;&#x308F;&#12369;&#12391;&#12418;&#xFF5E;&#x306A;&#12356;</trd>
  <trd lng="nl">om geen enkele <ind>reden</ind></trd>
  <trdgrp lng="pl">
    <trd>z &zdot;adnego <ind>powodu</ind></trd>,
    <trd>bez <ind>powodu</ind></trd>,
    <trd>bez <ind>przyczyny</ind></trd>
  </trdgrp>
  <trd lng="pt">por nenhum <klr tip="amb">motivo</klr></trd>
  <trdgrp lng="ro">
    <trd>f&abreve;r&abreve; nici un motiv</trd>,
    <trd>f&abreve;r&abreve; motiv</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_i;&c_p;&c_o;&c_ch;&c_je;&c_m;&c_u;</trd>,
    <trd>&c_n;&c_i;&c_z;&c_a;&c_ch;&c_je;&c_m;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_p;&c_r;&c_i;&c_ch;&c_i;&c_n;&c_n;&c_o;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_o; &c_t;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bezd&ocirc;vodne</trd>,
    <trd>pre ni&ccaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&#x43E;&c_s;&c_t;&#x43E; &c_t;&c_a;&#x43A;</trd>,
    <trd>&c_b;&c_je;&c_z;&#x43F;&c_r;&c_i;&c_ch;&c_i;&#x43D;&#x43D;&#x43E;</trd>,
    <trd>&c_b;&c_je;&c_z; &c_zh;&#x43E;&c_d;&#x43D;&#x43E;&c_ji; &#x43F;&c_r;&c_i;&c_ch;&c_i;&#x43D;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6C92;&#x4F86;&#30001; <pr>m&#xE9;il&#xE1;iy&#xF3;u</pr></trd>,
    <trd>&#x6CA1;&#26469;&#30001; <pr>m&#xE9;il&#xE1;iy&#xF3;u</pr></trd>,
    <trd>&#x65E0;&#x7AEF; <pr>w&#xFA;du&amacron;n</pr></trd>,
    <trd>&#28961;&#x7AEF; <pr>w&#xFA;du&amacron;n</pr></trd>
  </trdgrp>
</drv>
</art>
