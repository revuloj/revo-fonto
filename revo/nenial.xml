<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: nenial.xml,v 1.21 2018/08/10 16:10:15 revo Exp $">
<kap>
  <ofc>*</ofc><ofc>7</ofc>
  <rad>nenial</rad> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="nenial.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Adv. signifanta &leftquot;pro neniu ajn ka&ubreve;zo, motivo&rightquot;:
      <ekz>
Aldonante la prefikson &leftquot;nen&#8221;, ni ricevas vortojn neajn: nenia, nenial,
neniam, nenie, neniel, nenies, nenio, neniom, neniu.
      <fnt><bib>F</bib>, <lok>&FE; 30</lok></fnt>.
      </ekz>
      <ekz>
        <tld/> vi koleras;
      </ekz>
      <ekz>
        li <tld/> &ccirc;esis skribi al mi;
      </ekz>
      <ekz>
        ni <tld/> volas partopreni tiun agitadon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_je;&c_s;&c_p;&c_r;&c_y;&c_ch;&c_y;&c_n;&c_n;&c_a;</trd>,
    <trd>&c_b;&c_je;&c_z; &c_d;&c_a;&c_j; &c_p;&c_r;&c_y;&c_ch;&c_y;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bez p&rcaron;&iacute;&ccaron;iny</trd>,
    <trd>pro nic za nic</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>grundlos</trd>,
     <trd>aus keinem <ind>Grund</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>for no <ind>reason (cause, motive)</ind></trd>,
    <trd>not for any <ind>reason (cause, motive)</ind></trd>,
    <trd>on no <ind>account</ind></trd>,
    <trd>without any <ind>reason (cause, motive)</ind> whatever</trd>
  </trdgrp>
  <trd lng="es">por ninguna raz&oacute;n</trd>
  <trd lng="fr">pour aucune cause</trd>
  <trdgrp lng="hu">
    <trd>semmi&eacute;rt<klr tip="ind"> (semmilyen okb&oacute;l)</klr></trd>,
    <trd>semmilyen okn&aacute;n&aacute;l fogva</trd>
  </trdgrp>
  <trd lng="nl">om geen enkele <ind>reden</ind></trd>
  <trdgrp lng="pl">
    <trd>z &zdot;adnego <ind>powodu</ind></trd>,
    <trd>bez <ind>powodu</ind></trd>,
    <trd>bez <ind>przyczyny</ind></trd>
  </trdgrp>
  <trd lng="pt">por nenhum <klr tip="amb">motivo</klr></trd>
  <trdgrp lng="ro">
    <trd>f&abreve;r&abreve; nici un motiv</trd>,
    <trd>f&abreve;r&abreve; motiv</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_i;&c_p;&c_o;&c_ch;&c_je;&c_m;&c_u;</trd>,
    <trd>&c_n;&c_i;&c_z;&c_a;&c_ch;&c_je;&c_m;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_p;&c_r;&c_i;&c_ch;&c_i;&c_n;&c_n;&c_o;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_o; &c_t;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bezd&ocirc;vodne</trd>,
    <trd>pre ni&ccaron;</trd>
  </trdgrp>
</drv>
</art>
