<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: akvare.xml,v 1.28 2023/10/28 08:33:04 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>akvarel</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="akvare.0o">
  <kap><tld/>o</kap>
  <snc mrk="akvare.0o.KOL">
    <dif>
      Diafana pentra <ref tip="super" cel="farb.0o.KOL">farbo</ref>
      solvebla en akvo kun iom da akvosolvebla ligento:
      <ekz>
        li montris al ili per <tld/>aj pentra&jcirc;oj kelkajn &gcirc;iajn erupciojn
        <fnt>
          <bib>Fab4</bib>
          <lok>&leftquot;Bela&rightquot;</lok>
        </fnt>.
      </ekz>
    </dif>
<ref tip="vid" cel="lav.0umi">lavumi</ref>
    <trdgrp lng="zh">
    <trd>&#x6C34;&#x5F69; <pr>shu&#x1D0;c&#x1CE;i</pr></trd>,
    <trd>&#x6C34;&#x58A8; <pr>shu&#x1D0;m&#xF2;</pr></trd>,
    <trd>&#x6C34;&#x5F69;&#x984F;&#x6599; <pr>shu&#x1D0;c&#x1CE;iy&#xE1;nli&#xE0;o</pr></trd>,
    <trd>&#x6C34;&#x5F69;&#x989C;&#x6599; <pr>shu&#x1D0;c&#x1CE;iy&#xE1;nli&#xE0;o</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="akvare.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      <ref tip="super" cel="pentr.0ajxo">Pentra&jcirc;o</ref> sur papero farita per <tld/>aj koloriloj:
      <ekz>
        Arne <klr>[&#x2026;]</klr> metis sur la tablon <klr>[&#x2026;]</klr> amason da paperoj kun
        desegna&jcirc;oj kaj <tld/>oj, kelkajn skizlibrojn kaj dekon da olepentra&jcirc;oj
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        flandra kolekto enhavanta pli ol 1580 naturhistoriajn
        <tld/>ojn<fnt><aut>Johan Van Eenoo</aut>: <vrk>La bru&gcirc;a
        Centuriae
        plantarum rariorum retrovita en Krakovo</vrk>,
        <bib>Monato</bib><lok>jaro 1998a, numero 1a, p. 16a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x6C34;&#x5F69;&#x753B; <pr>shu&#x1D0;c&#x1CE;ihu&#xE0;</pr></trd>,
    <trd>&#x6C34;&#x5F69;&#x756B; <pr>shu&#x1D0;c&#x1CE;ihu&#xE0;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_k;&c_v;&c_a;&c_r;&c_e;&c_l;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>akvarel</trd>,
    <trd>akvarelov&yacute; obraz</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Aquarell</trd>,
     <trd>Gem&auml;lde in <ind>Wasserfarben</ind></trd>
  </trdgrp>
  <trd lng="en">aquarelle</trd>
  <trd lng="es">acuarela</trd>
  <trd lng="fa">&a_A_madda;&a_b;&a_r;&a_n;&f_g;</trd>
  <trd lng="fr">aquarelle</trd>
  <trd lng="he">&tsadi;&#x5B4;&bet;&ayin;&#x5B5;&yod; &mem;&#x5B7;&yod;&#x5B4;&fmem;</trd>
  <trd lng="hu">akvarell</trd>
  <trd lng="it">acquarello</trd>
  <trd lng="ja">&#x6C34;&#x5F69;&#x753B;  <pr>&#x3059;&#x3044;&#x3055;&#x3044;&#x304C;</pr></trd>
  <trd lng="nl">aquarel</trd>
  <trd lng="pt">aquarela</trd>
  <trd lng="ru">&c_a;&c_k;&c_v;&c_a;&c_r;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">akvarel</trd>
  <trd lng="sv">akvarell</trd>
  <trd lng="uk">&c_a;&c_k;&c_v;&c_a;&c_r;&c_je;&c_l;&c_mol; (&c_k;&c_a;&c_r;&c_t;&c_i;&c_n;&c_a;)</trd>
</drv>
</art>
