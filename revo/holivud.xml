<art mrk="$Id: holivud.xml,v 1.5 2023/11/15 17:08:01 revo Exp $">
<kap>
  <rad>Holivud</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="holivud.0o">
  <kap><tld/>o</kap>
  <snc mrk="holivud.0o.KIN">
    <uzo tip="fak">KIN</uzo>
    <dif>
      La usona filmindustrio kaj kinomedio &gcirc;enerale,
      kaj precipe tiuj el la samnoma kvartalo de Losan&gcirc;eleso:
      <ekz>
        en <tld lit="h"/>o, estas malmultaj &scirc;ancoj por sukceso
        de aziaj aktoroj
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url ref="http://es1.chinabroadcast.cn/221/2007/04/05/1@57551.htm">
          &Ccirc;ina Re&gcirc;isoro Chen Daming</url></vrk>,
          <lok>2007-04-05</lok>
        </fnt>;
      </ekz>
      <ekz>
        fantomoj kun simpla blanka littuko parolas la <tld lit="h"/>an lingvon
        kaj provas timigi filmspektantojn
        <fnt>
          <aut>Berto InformFantomix</aut>,
          <vrk>Fantomoj svarmis dum silvestra nokto</vrk>,
          <bib>LOdE</bib>,
          <lok>2011:2 (196)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_G;&c_a;&c_l;&c_ib;&c_v;&c_u;&c_d;</trd>
  <trd lng="bg">&c_H;&c_o;&c_l;&c_i;&c_v;&c_u;&c_d;</trd>
  <trd lng="de">Hollywood</trd>
  <trd lng="en">Hollywood</trd>
  <trd lng="fr">Hollywood</trd>
  <trd lng="nl">Hollywood</trd>
  <trd lng="ru">&c_G;&c_o;&c_l;&c_l;&c_i;&c_v;&c_u;&c_d;</trd>
  <trdgrp lng="zh">
    <trd>&#x597D;&#x83B1;&#x575E; <pr>H&#x1CE;ol&#xE1;iw&#xF9;</pr></trd>,
    <trd>&#x597D;&#x83DC;&#x575E; <pr>h&#x1CE;oc&#xE0;iw&#xF9;</pr></trd>,
    <trd>&#x597D;&#x83B1;&#x575E; <pr>h&#x1CE;ol&#xE1;iw&#x16B;</pr></trd>
  </trdgrp>
</drv>
