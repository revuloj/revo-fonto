<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>klap</rad>/o
</kap>
<drv mrk="klap.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Diversforma kvaza&ubreve;porda &scirc;topilo el ligno a&ubreve; metalo,
      destinita por enlasi (a&ubreve; ellasi) flua&jcirc;on a&ubreve; gason. 
    </dif>
    <trd lng="de">Klappe</trd>
    <trd lng="en">valve</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_l;&c_a;&c_p;&c_a;&c_n;</trd>,
      <trd>&c_z;&c_a;&c_s;&c_l;&c_o;&c_n;&c_k;&c_a;</trd>,
      <trd>&c_v;&c_je;&c_n;&c_t;&c_i;&c_l;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <dif>
      &Ccirc;iu el la moveblaj partoj de dupeca konko; valvo. 
    </dif>
    <trdgrp lng="en">
      <trd>half shell</trd>,
      <trd>valve</trd>
    </trdgrp>
    <trd lng="de">Ventil</trd>
    <trd lng="ru">&c_s;&c_t;&c_v;&c_o;&c_r;&c_k;&c_a;</trd>
  </snc>
  <snc num="3">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Moderigilo:
      <ekz>mankas <tld/>o en lia kapo
        <klr>(li estas malsa&gcirc;a)</klr><fnt><bib>PrV</bib></fnt>.</ekz> 
    </dif>
  </snc>
<trdgrp lng="nl">
   <trd>klep</trd>,
         <trd>ventiel</trd>
         </trdgrp>
</drv>
<drv mrk="klap.0ego">
  <kap><tld/>ego</kap>
  <snc>
    <dif>
      Movebla parto de pordo, pordoflugilo. 
    </dif>
  </snc>
  <trd lng="de">T&uuml;rangel</trd>
  <trd lng="ru">&c_s;&c_t;&c_v;&c_o;&c_r;&c_k;&c_a; <klr>(&c_d;&c_v;&c_je;&c_r;&c_i;)</klr></trd>
</drv>
<drv mrk="klap.sav0o">
  <kap>sav<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="K"/>o, kiu en kaldronego de vaporma&scirc;ino malfermi&gcirc;as
      per si mem sub forta premo, por lasi eli&gcirc;i parton de la vaporo,
      kaj malhelpi tiel eksplodon de la kaldronego:
      (f) <ekz>la vulkanoj estas la sav<tld/>oj de la tera globo.</ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Schutzklappe</trd>,
     <trd>&Uuml;berdruckventil</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>pop-off valve</trd>,
    <trd>safety valve</trd>
  </trdgrp>
  <trdgrp lng="nl">
     <trd>veiligheidsklep</trd>,
           <trd>veiligheidsventiel</trd>
           </trdgrp>
  <trd lng="ru">&c_k;&c_l;&c_a;&c_p;&c_a;&c_n;</trd>
</drv>
</art>
