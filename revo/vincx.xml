<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>vin&ccirc;</rad>/o
</kap>
<drv mrk="vincx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Horizontala bobenego sur kiu oni volvas, per mana a&ubreve; mekanika
      turnado, kablon por suprenlevi a&ubreve; tiri ion.
    </dif>
    <refgrp tip="sin">
      <ref cel="kapsta.0o">kapstano</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Winde</trd>
  <trdgrp lng="nl">
    <trd>windas</trd>,
    <trd>lier</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_je;&c_b;&c_jo;&c_d;&c_k;&c_a;</trd>
</drv>
</art>
