<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>abscis</rad>/o
</kap>
<drv mrk="abscis.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PV</bib></fnt>
  <snc>
    <dif>
      La unua el la du
      <ref tip="super" cel="koordi.kartezia0o">karteziaj koordinatoj</ref>,
      kiuj difinas la situon de punkto sur
      <ref cel="eben.0o.MAT">ebeno</ref>:
      <ekz>
        la <tld/>on de punkto oni kutime signas per la litero x;
      </ekz>
      <ekz>
        &ccirc;e grafika&jcirc;o de funkcio de unu argumento, la <tld/>o
        reprezentas la argumenton.
      </ekz>
    </dif>
    <ref tip="vid" cel="ordina1.0o">ordinato.</ref>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_s;&c_c;&c_y;&c_s;&c_a;</trd>
  <trdgrp lng="nl">
    <trd>abscis</trd>,
    <trd>x-co&ouml;rdinaat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Abszisse</trd>,
    <trd>x-Koordinate</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>abscissa</trd>,
    <trd>x-coordinate</trd>
  </trdgrp>
  <trd lng="es">abscisa</trd>
  <trdgrp lng="fr">
    <trd>abscisse</trd>,
    <trd>coordonn&eacute;e x</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>abszcissza</trd>,
    <trd>v&iacute;zszintes <ind>tengely</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>odci&eogonek;ta</trd>,
    <trd>abscysa</trd></trdgrp> 
  <trd lng="pt">abscissa</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_b;&c_s;&c_c;&c_i;&c_s;&c_s;&c_a;</trd>,
    <trd>x-&c_k;&c_o;&c_o;&c_r;&c_d;&c_i;&c_n;&c_a;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">x-ov&aacute; s&uacute;radnica</trd>
  <trdgrp lng="sv">
    <trd>abskissa</trd>,
    <trd>x-koordinat</trd>
  </trdgrp>
</drv>
</art>
