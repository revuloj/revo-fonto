<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kontagx.xml,v 1.12 2025/11/16 15:48:03 revo Exp $">
<kap>
  <rad>konta&gcirc;</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="kontagx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kontagx.0i.MED">
    <dif>
      <ref tip="super" cel="infekt.0i.MED">Infekti</ref> alian vivulon
      transdonante al &gcirc;i propran malsanon:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li <tld/>is siajn lernantojn per tiu humoro
        <fnt>
          <aut>M. Bijleveld</aut>,
          <vrk><url ref="http://www.kke.org.br/esperanto/prelegoj/andreo_cxe_instruis_idiomon_gxojigante_la_homojn.php">
          Andreo &Ccirc;e instruis...</url></vrk>,
          <lok>Kultura Kooperativo de Esperantistoj, [1993]</lok>
        </fnt>;
      </ekz>
      <ekz>
        nia armeo alportis de la turka armeo &hcirc;oleron, <tld/>is
        &gcirc;in al la rumanoj, kiuj &gcirc;in transportis al la apudaj
        vila&gcirc;oj
        <fnt>
          <aut>T. &Hcirc;ristovski, trad. N. Aleksiev</aut>,
          <vrk><url ref="http://miresperanto.narod.ru/biblioteko/hhristovski.htm">
          Mia vivo</url></vrk>,
          <lok>Sofia Pres, 1981</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_r;&c_a;&c_zh;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>anstecken <klr>(Krankheit)</klr></trd>,
    <trd>&uuml;bertragen <klr>(Krankheit)</klr></trd>,
    <trd>infizieren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>contaminate</trd>,
    <trd>propagate</trd>
  </trdgrp>
  <trd lng="fr">contaminer<klr> (causer une contagion)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x63A5;&#x89E6;&#x611F;&#x67D3;&#x3055;&#x305B;&#x308B;  <pr>&#x305B;&#x3063;&#x3057;&#x3087;&#x304F;&#x304B;&#x3093;&#x305B;&#x3093;&#x3055;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x4F1D;&#x67D3;&#x3055;&#x305B;&#x308B;  <pr>&#x3067;&#x3093;&#x305B;&#x3093;&#x3055;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">besmetten</trd>
  <trdgrp lng="pl">
    <trd>zara&zdot;a&cacute;</trd>,
    <trd>zaka&zdot;a&cacute;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x67D3; <pr>r&#x1CE;n</pr></trd>,
    <trd>&#x4F20;&#x67D3; <pr>chu&aacute;nr&#x1CE;n</pr></trd>,
    <trd>&#x611F;&#x67D3; <pr>g&#x1CE;nr&#x1CE;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kontagx.0a">
  <kap><tld/>a</kap>
  <snc mrk="kontagx.0a.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Povanta infekti per <tld/>o:
      <ekz>
        personoj kun <tld/>aj malsanoj ne rajtas eniri la bibliotekon
        <fnt>
          <aut>Germana Esperanto-Biblioteko Aleno</aut>,
          <vrk><url ref="http://esperanto-bibliothek.gmxhome.de/ben_ord/regul-u1.htm">
           Uzregularo konciza</url></vrk>,
          <lok>1996</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_r;&c_a;&c_z;&c_mol;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_z;&c_a;&c_r;&c_a;&c_z;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bo">&t_-a;&t_ga;&t_naro;&t_sa;&t_tsheg;&t_sa;&t_lata;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>
  <trdgrp lng="de">
    <trd>ansteckend</trd>,
    <trd>&uuml;bertragbar</trd>,
    <trd>virulend</trd>,
    <trd>infekti&ouml;s</trd>,
    <trd>kontagi&ouml;s</trd>
  </trdgrp>
  <trd lng="en">contagious</trd>
  <trd lng="fr">contagieux</trd>
  <trdgrp lng="ja">
    <trd>&#x63A5;&#x89E6;&#x611F;&#x67D3;&#x306E;  <pr>&#x305B;&#x3063;&#x3057;&#x3087;&#x304F;&#x304B;&#x3093;&#x305B;&#x3093;&#x306E;</pr></trd>,
    <trd>&#x4F1D;&#x67D3;&#x6027;&#x306E;  <pr>&#x3067;&#x3093;&#x305B;&#x3093;&#x305B;&#x3044;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">besmettend</trd>
  <trdgrp lng="pl">
    <trd>zara&zacute;liwy</trd>,
    <trd>zaka&zacute;ny</trd>
  </trdgrp>
</drv>

<drv mrk="kontagx.0o">
  <kap><tld/>o</kap>
  <snc mrk="kontagx.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Transdonado de malsano:
      <ekz>
        anta&ubreve;zorgoj limigis pluan disvasti&gcirc;on de la <tld/>o
        en tiu loko
        <fnt><aut>A. Manzoni, trad. B. Cadei</aut>, <vrk>La
        Gefian&ccirc;oj</vrk>, <lok>FEI, 2006</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="epidem.0o.MED">epidemio</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_r;&c_a;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_z;&c_a;&c_r;&c_a;&c_z;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&t_-a;&t_ga;&t_naro;&t_sa;&t_tsheg;&t_na;&t_da;&t_tsheg;</trd>
  <trdgrp lng="de">
    <trd>Ansteckung</trd>,
    <trd>Weitergabe <klr>(Krankheit)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>contamination</trd>,
    <trd>contagion</trd>
  </trdgrp>
  <trd lng="fr">contagion</trd>
  <trdgrp lng="ja">
    <trd>&#x63A5;&#x89E6;&#x611F;&#x67D3;  <pr>&#x305B;&#x3063;&#x3057;&#x3087;&#x304F;&#x304B;&#x3093;&#x305B;&#x3093;</pr></trd>,
    <trd>&#x4F1D;&#x67D3;  <pr>&#x3067;&#x3093;&#x305B;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">besmetting</trd>
  <trdgrp lng="pl">
    <trd>zaka&zdot;enie</trd>,
    <trd>zaraza</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6CBE;&#x67D3; <pr>zh&amacron;nr&#x1CE;n</pr></trd>,
    <trd>&#x611F;&#x67D3; <pr>g&#x1CE;nr&#x1CE;n</pr></trd>
  </trdgrp>
</drv>
</art>

