<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nadl.xml,v 1.21 2023/09/02 12:44:19 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>nadl</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="nadl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Pintoforma metala vergeto:
      <ekz>
        <tld/>o de elektra aparato;
      </ekz>
      <ekz>
        magneta <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kudr.0ilo">kudrilo</ref>,
      <ref cel="montr.0ilo">montrilo</ref>,
      <ref cel="pingl.0o">pinglo</ref>,
      <ref cel="pint.0o">pinto</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_g;&c_o;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_e;&c_l;&c_k;&c_a; <klr>(&c_n;&c_a;&c_p;&c_r;.,
    &c_g;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_mol;&c_n;&c_ib;&c_k;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="br">nadoz <klr tip="ind">(nemet evit gwiat)</klr></trd>
  <trdgrp lng="ca">
    <trd>agulla</trd>,
    <trd>punxa</trd>,
    <trd>pua</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>jehla</trd>,
    <trd>st&rcaron;elka</trd>
  </trdgrp>
  <trd lng="de">Nadel</trd>
  <trdgrp lng="en">
    <trd>needle</trd>,
    <trd>indicator</trd>,
    <trd>hand</trd>,
    <trd>pointer</trd>
  </trdgrp>
  <trd lng="es">aguja<klr> (de coser)</klr></trd>
  <trd lng="fr">aiguille <klr tip="ind">(sauf pour coudre)</klr></trd>
  <trd lng="hu">t&udblac;</trd>
  <trdgrp lng="ja">
    <trd>&#x91DD;  [<ind>&#x306F;&#x308A;</ind>]</trd>,
    <trd>&#x6307;&#x91DD;  [<ind>&#x3057;&#x3057;&#x3093;</ind>]</trd>,
    <trd>&#x78C1;&#x91DD;  [<ind>&#x3058;&#x3057;&#x3093;</ind>]</trd>,
    <trd>&#x6483;&#x91DD;  [<ind>&#x3052;&#x304D;&#x3057;&#x3093;</ind>]</trd>,
    <trd>&#x30A8;&#x30AE;&#x30FC;&#x30E6;</trd>
  </trdgrp>
  <trd lng="nl">naald</trd>
  <trd lng="pt">agulha</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_g;&c_o;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_je;&c_l;&c_k;&c_a; <klr>(&c_ch;&c_a;&c_s;&c_o;&c_v; &c_i; &c_t;.&c_p;.)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ihla</trd>,
    <trd>strelka</trd>
  </trdgrp>
  <trd lng="tr">i&gbreve;ne</trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x442;&#x440;&#x456;&#x43B;&#x43A;&#x430; (&#x433;&#x43E;&#x434;&#x438;&#x43D;&#x43D;&#x438;&#x43A;&#x430;</trd>,
    <trd>&#x441;&#x435;&#x43A;&#x443;&#x43D;&#x434;&#x43E;&#x43C;&#x456;&#x440;&#x430;</trd>,
    <trd>&#x43A;&#x43E;&#x43C;&#x43F;&#x430;&#x441;&#x430; &#x456; &#x442;.&#x43F;.)</trd>,
    <trd>&#x433;&#x43E;&#x43B;&#x43A;&#x430; (&#x43F;&#x440;&#x43E;&#x433;&#x440;&#x430;&#x432;&#x430;&#x447;&#x430; &#x456; &#x442;.&#x43F;.)</trd>
  </trdgrp>
</drv>
</art>
