<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>nadl</rad>/o
</kap>
<drv mrk="nadl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Pintoforma metala vergeto:
      <ekz>
        <tld/>o de elektra aparato;
      </ekz>
      <ekz>
        magneta <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kudr.0ilo">kudrilo</ref>,
      <ref cel="montr.0ilo">montrilo</ref>,
      <ref cel="pingl.0o">pinglo</ref>,
      <ref cel="pint.0o">pinto</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_g;&c_o;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_e;&c_l;&c_k;&c_a; <klr>(&c_n;&c_a;&c_p;&c_r;., &c_g;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_mol;&c_n;&c_ib;&c_k;&c_a;)</klr></trd>
  </trdgrp>                  
  <trd lng="br">nadoz <klr tip="ind">(nemet evit gwiat)</klr></trd>
  <trd lng="de">Nadel</trd>
  <trdgrp lng="en">
    <trd>needle</trd>,
    <trd>indicator</trd>,
    <trd>hand</trd>,
    <trd>pointer</trd>
  </trdgrp>      
  <trd lng="es">aguja<klr> (de coser)</klr></trd>
  <trd lng="fr">aiguille <klr tip="ind">(sauf pour coudre)</klr></trd>
  <trd lng="hu">t&udblac;</trd>
  <trd lng="nl">naald</trd>
  <trd lng="pt">agulha</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_g;&c_o;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_je;&c_l;&c_k;&c_a; <klr>(&c_ch;&c_a;&c_s;&c_o;&c_v; &c_i; &c_t;.&c_p;.)</klr></trd>
  </trdgrp>
</drv>
</art>
