<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bandolier.xml,v 1.4 2024/02/19 09:24:03 revo Exp $">
<kap>
  <rad>bandolier</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="bandolier.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="bandolier.0o.KOMUNE">
    <dif>
      <ref tip="dif" cel="balte.0o">Balteo</ref>:
      <ekz>
        Filipo a&ccirc;etis &ccirc;e la vaganta &jcirc;onglisto <klr>[&#8230;]</klr>
        &ccirc;iujn liajn skarpojn kaj zonojn, jupojn kaj <tld/>ojn
        <fnt>
          <aut>F. Ko&zcaron;&iacute;k, trad. J. Karen kaj J. Vondrou&scaron;ek</aut>,
          <vrk>La plej eminenta inter Pierotoj</vrk>,
          <lok>2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_t;&c_u;&c_p;&c_e;&c_ja;</trd>,
    <trd>&c_t;&c_a;&c_l;&c_mol;&c_m;&c_a;&c_h;</trd>,
    <trd>&c_v;&c_ja;&c_z;&c_mol;&c_m;&c_o;</trd>
  </trdgrp>
    <trd lng="de">Bandelier</trd>
  <trd lng="fr">bandouli&egrave;re</trd>
</drv>
</art>

