<kap>
  <rad>rampl</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="rampl.0o">
  <kap><tld/>o</kap>
  <snc mrk="rampl.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref cel="dekliv.0o">Dekliva</ref> veturejo aran&gcirc;ita inter du malsame altaj
      lokoj:
      <ekz>
        <nom>Arabela</nom> <klr>[&#8230;]</klr> estis grimpanta sur la
        <tld/>o, kiu kondukis al la nedifektebla, eterna kaj
        grandioza statuo
        <fnt><aut>S. Larbar</aut>, <vrk>Karuseloj</vrk>,
        <lok>1987</lok></fnt>.
      </ekz>
      <ekz>
        li &ccirc;irka&ubreve;iris al la kontra&ubreve;a flanko de l' spaco&scirc;ipo, kie algrundi&gcirc;is <tld/>o
        <fnt>
          <aut>Harry Harrison, trad. William Auld </aut>
          <vrk><url ref="https://esperanto-ondo.ru/Ascalon.htm">La Stratoj de A&scirc;kelono</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>dekliva&#309;o</ke>, <ke>dekliva vojo</ke>, <ke>dekliva tabulo</ke>, <ke>en/elira vojo</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_m;&c_p;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_d;&c_u;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>naklon&ecaron;n&aacute; plo&scaron;ina</trd>,
    <trd>rampa</trd>
  </trdgrp>
  <trd lng="de">Rampe</trd>
  <trdgrp lng="fr">
    <trd>plan inclin&eacute;</trd>,
    <trd>rampe<klr> (voie)</klr></trd>
  </trdgrp>
  <trd lng="pl">rampa  <klr>(budownictwo)</klr></trd>
  <trd lng="ro">ramp&abreve;</trd>
  <trdgrp lng="sk">
    <trd>naklonen&aacute; plo&scaron;ina</trd>,
    <trd>rampa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_a;&c_n;&c_d;&c_u;&c_s;</trd>,
    <trd>&c_r;&c_a;&c_m;&c_p;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#28779;&#31661;&#21457;&#23556;&#21488; <pr>hu&#466;ji&#224;nf&#257;sh&#232;t&#225;i</pr></trd>,
    <trd>&#28779;&#31661;&#30332;&#23556;&#33274; <pr>hu&#466;ji&#224;nf&#257;sh&#232;t&#225;i</pr></trd>,
    <trd>&#33310;&#21488;&#21069;&#27839; <pr>w&#468;t&#225;iqi&#225;ny&#225;n</pr></trd>,
    <trd>&#35013;&#21368;&#21488; <pr>zhu&#257;ngxi&#232;t&#225;i</pr></trd>,
    <trd>&#35037;&#21368;&#21488; <pr>zhu&#257;ngxi&#232;t&#225;i</pr></trd>
  </trdgrp>
</drv>
</art>

