<art mrk="$Id: rampl.xml,v 1.9 2024/04/13 20:15:51 revo Exp $">
<kap>
  <rad>rampl</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="rampl.0o">
  <kap><tld/>o</kap>
  <snc mrk="rampl.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref cel="dekliv.0o">Dekliva</ref> veturejo aran&gcirc;ita inter du malsame altaj
      lokoj:
      <ekz>
        <nom>Arabela</nom> <klr>[&#x2026;]</klr> estis grimpanta sur la
        <tld/>o, kiu kondukis al la nedifektebla, eterna kaj
        grandioza statuo
        <fnt><aut>S. Larbar</aut>, <vrk>Karuseloj</vrk>,
        <lok>1987</lok></fnt>.
      </ekz>
      <ekz>
        li &ccirc;irka&ubreve;iris al la kontra&ubreve;a flanko de l' spaco&scirc;ipo, kie algrundi&gcirc;is <tld/>o
        <fnt>
          <aut>Harry Harrison, trad. William Auld </aut>
          <vrk><url ref="https://esperanto-ondo.ru/Ascalon.htm">La Stratoj de A&scirc;kelono</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_m;&c_p;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_d;&c_u;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>naklon&ecaron;n&aacute; plo&scaron;ina</trd>,
    <trd>rampa</trd>
  </trdgrp>
  <trd lng="de">Rampe</trd>
  <trdgrp lng="fr">
    <trd>plan inclin&eacute;</trd>,
    <trd>rampe<klr> (voie)</klr></trd>
  </trdgrp>
  <trd lng="pl">rampa  <klr>(budownictwo)</klr></trd>
  <trd lng="ro">ramp&abreve;</trd>
  <trdgrp lng="sk">
    <trd>naklonen&aacute; plo&scaron;ina</trd>,
    <trd>rampa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_a;&c_n;&c_d;&c_u;&c_s;</trd>,
    <trd>&c_r;&c_a;&c_m;&c_p;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x706B;&#x7BAD;&#x53D1;&#x5C04;&#x53F0; <pr>hu&#x1D2;ji&#xE0;nf&#x101;sh&#xE8;t&#xE1;i</pr></trd>,
    <trd>&#x706B;&#x7BAD;&#x767C;&#x5C04;&#x81FA; <pr>hu&#x1D2;ji&#xE0;nf&#x101;sh&#xE8;t&#xE1;i</pr></trd>,
    <trd>&#x821E;&#x53F0;&#x524D;&#x6CBF; <pr>w&#x1D4;t&#xE1;iqi&#xE1;ny&#xE1;n</pr></trd>,
    <trd>&#x88C5;&#x5378;&#x53F0; <pr>zhu&#x101;ngxi&#xE8;t&#xE1;i</pr></trd>,
    <trd>&#x88DD;&#x5378;&#x53F0; <pr>zhu&#x101;ngxi&#xE8;t&#xE1;i</pr></trd>
  </trdgrp>
</drv>
</art>

