<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: frok.xml,v 1.7 2023/02/16 07:26:32 revo Exp $">
<kap>
  <rad>frok</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="frok.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Lar&gcirc;a kapu&ccirc;a <ref tip="super" cel="mantel.0o">mantelo</ref>, pliofte
      &gcirc;ispiede longa, moda en frumezepoka E&ubreve;ropo, poste tipa mona&hcirc;a
      survesto:
      <ekz>
        li desaltis de la azeno, sin &jcirc;etis al li kaj komencis detiri de li
        la <tld/>on
        <fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 8a, p. 70a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="burnus.0o">burnuso</ref>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_a; <klr tip="amb">(&c_v;&c_o;&c_p;&c_r;&c_a;&c_t;&c_k;&c_a;)</klr></trd>
  <trd lng="cs">mni&scaron;sk&#xE1; kutna</trd>
  <trd lng="fr">froc</trd>
  <trd lng="hu">csuha</trd>
  <trd lng="pt">h&#xE1;bito <klr tip="amb">(de frade)</klr></trd>
  <trd lng="sk">mn&#xED;&scaron;ska kut&ncaron;a</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&#x44F;&c_s;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_t;&c_a;&#x43D;&c_a;</trd>
  </trdgrp>
</drv>
</art>

