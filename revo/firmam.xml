<vortaro>
<art mrk="$Id$">
<kap>
  <rad>firmament</rad>/o
</kap>
<drv mrk="firmam.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      &Ccirc;iela volbo:
      <ekz>
        li eliras de la sunsistemo en la vastan <tld/>on<fnt>
        <aut>Garbhan Macaoidh</aut>:
        <vrk>Per ardua ad astra - Tra kruta&jcirc;oj al la
        astroj (David Galadi-Enriquez kaj Amri Wandel: La kosmo kaj
        ni)</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 6a, p.
        17a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_b;&c_a;&c_s;&c_h;&c_ib;&c_l;</trd>
  <trdgrp lng="de">
    <trd>Himmelszelt</trd>,
    <trd>Firmament</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>firmament</trd>,
    <trd>vo&ucirc;te c&eacute;leste</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;gbolt</trd>,
    <trd>mennybolt</trd>
  </trdgrp>
  <trd lng="nl">firmament</trd>
  <trdgrp lng="pl">
    <trd>firmament</trd>,
    <trd>sklepienie niebieskie</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>firmamento</trd>,
    <trd>ab&oacute;boda celeste</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_b;&c_o;&c_s;&c_v;&c_o;&c_d;</trd>,
    <trd>
    &c_n;&c_je;&c_b;&c_je;&c_s;&c_n;&c_a;&c_ja;
    &c_t;&c_v;&c_je;&c_r;&c_d;&c_mol;
    </trd>
  </trdgrp>
</drv>
</art>
