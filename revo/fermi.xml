<vortaro>
<art mrk="$Id$">
<kap>
  <rad>fermi</rad>/o
</kap>
<drv mrk="fermi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
     &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
     atomnumero 100, atompezo 257,0951, simbolo Fm. <ref tip="super"
     cel="urani.trans0o">Transuranio</ref>. &Ccirc;iuj izotopoj de tiu
elemento
     estas radioaktivaj.
    </dif>
  </snc>
  <trd lng="de">Fermium</trd>
  <trd lng="en">fermium</trd>
  <trd lng="hu">fermium</trd>
  <trd lng="nl">fermium</trd>
  <trd lng="ru">&c_f;&c_je;&c_r;&c_m;&c_i;&c_j;</trd>
</drv>
</art>

