<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>fermi</rad>/o
</kap>
<drv mrk="fermi.0o">
  <kap><tld/>o</kap>
  <mlg>Fm</mlg>
  <snc mrk="fermi.0o.KEM">
  <uzo tip="fak">KEM</uzo>
    <dif>
     &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref>,
     <ref tip="super"      cel="urani.trans0o">transuranio</ref> kun
     atomnumero 100, atompezo 257,0951, simbolo <ctl>Fm</ctl>.
     &Ccirc;iuj izotopoj de tiu elemento     estas radioaktivaj.
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_e;&c_r;&c_m;&c_ib;&c_j;</trd>
  <trd lng="de">Fermium</trd>
  <trd lng="en">fermium</trd>
  <trd lng="fr">fermium</trd>
  <trd lng="hu">fermium</trd>
  <trd lng="nl">fermium</trd>
  <trd lng="ru">&c_f;&c_je;&c_r;&c_m;&c_i;&c_j;</trd>
</drv>
</art>

