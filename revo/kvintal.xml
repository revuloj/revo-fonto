<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kvintal</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kvintal.0o">
  <kap><tld/>o</kap>
  <snc mrk="kvintal.0o.MEZ">
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de
      <ref cel="mas.0o.FIZ">maso</ref> por peso de grandkvantaj 
      komerca&jcirc;oj, diversvalora la&ubreve; la landoj:
      <ekz>
        Sankta Anton&ccirc;jo nun helpu &ndash; li komencis pre&gcirc;i en
si, 
        &ndash; <tld/>on da kandeloj mi bruligos por vi post hejmreveno
        <fnt>
          <aut>Julio Baghy</aut>,
          <vrk><url
ref="http://donh.best.vwh.net/Esperanto/Literaturo/Noveloj/Kiel_Mihok_Instru
is_Angle.html">Kiel Mihok instruis angle</url></vrk>,
          <lok>Dancu, marionetoj. - Budape&scirc;to, 1927</lok>
        </fnt>.
      </ekz>
    </dif>
    <subsnc mrk="kvintal.0o.MEZ.funta">
      <dif>
        Cent a&ubreve; centdekdu <ref tip="prt"
        cel="funt.0o.MEZ">funtoj</ref>, proksimume kvindek
        kilogramoj.
      </dif>
      <trd lng="pl">kwintal</trd>
    </subsnc>
    <subsnc mrk="kvintal.0o.MEZ.kilograma">
      <dif>
        Cent kilogramoj:
        <ekz>
          449 <tld/>oj en &ccirc;iu areo de 135 kvadrataj kilometroj
          <fnt>
            <aut>E. Ramos</aut>,
            <vrk><url
ref="http://www.radiosanctispiritus.cu/Version%20Esperanto/Noticias/otrosTem
as/dic05/29/recuento.htm">...horo de la resumo</url></vrk>,
            <lok>Radio Sancti Sp&iacute;ritus, 2006-05-15</lok>
          </fnt>.
        </ekz>
      </dif>
	  <trdgrp lng="pl">
	   <trd>cetnar</trd>,
	   <trd>centnar</trd>
	  </trdgrp>
    </subsnc>
  </snc>
  <trd lng="fr">quintal</trd>
  <trd lng="pt">quintal</trd>
</drv>
</art>

