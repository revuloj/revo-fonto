<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kvintal.xml,v 1.14 2023/06/20 18:27:51 revo Exp $">
<kap>
  <rad>kvintal</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kvintal.0o">
  <kap><tld/>o</kap>
  <snc mrk="kvintal.0o.MEZ">
    <dif>
      <ref tip="lst" cel="unu.mezur0o" lst="voko:kvantoj">Mezurunuo</ref> de
      <ref cel="mas.0o.FIZ">maso</ref> de grandkvantaj
      komerca&jcirc;oj, diversvalora la&ubreve; la landoj:
      <ekz>
        Sankta Anton&ccirc;jo nun helpu &ndash; li komencis pre&gcirc;i en si,
        &ndash; <tld/>on da kandeloj mi bruligos por vi post hejmreveno
        <fnt>
          <aut>Julio Baghy</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Noveloj/Kiel_Mihok_Instruis_Angle.html">Kiel
          Mihok instruis angle</url></vrk>,
          <lok>Dancu, marionetoj. - Budape&scirc;to, 1927</lok>
        </fnt>.
      </ekz>
    </dif>
    <subsnc mrk="kvintal.0o.MEZ.funta">
      <dif>
        Cent a&ubreve; cent dek du <ref tip="prt" cel="funt.0o.MEZ">funtoj</ref>, proksimume kvindek
        kilogramoj.
      </dif>
      <trdgrp lng="de">
       <trd>Zentner</trd>,
       <trd>Quintal</trd>
      </trdgrp>
      <trd lng="pl">kwintal</trd>
    </subsnc>
    <subsnc mrk="kvintal.0o.MEZ.kilograma">
      <dif>
        Cent kilogramoj:
        <ekz>
          449 <tld/>oj en &ccirc;iu areo de 135 kvadrataj kilometroj
          <fnt>
            <aut>E. Ramos</aut>,
            <vrk><url
            ref="http://www.radiosanctispiritus.cu/Version%20Esperanto/Noticias/otrosTemas/dic05/29/recuento.htm">...horo
            de la resumo</url></vrk>,
            <lok>Radio Sancti Sp&iacute;ritus, 2006-05-15</lok>
          </fnt>;
        </ekz>
        <ekz>
          Hao esperas havigi &ccirc;irka&ubreve; 45 <tld/>ojn&#8239;/&#8239;ha el la rizo priklita en marto
          <fnt>
       <aut>Maryvonne kaj Bruno Robineau</aut>
          <vrk><url ref="&Monato;/2000/006831.php">La&ubreve; ritmo de la rizejoj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/02, p. 18</lok>
        </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="pl">
        <trd>cetnar</trd>,
        <trd>centnar</trd>
      </trdgrp>
    </subsnc>
  </snc>
  <trd lng="cs">metrick&yacute; cent <klr>(100 kg)</klr></trd>
  <trd lng="fr">quintal</trd>
  <trd lng="id">kuintal</trd>
  <trd lng="nl">kwintaal</trd>
  <trd lng="pt">quintal <klr>(unidade de massa)</klr></trd>
  <trdgrp lng="sk">
    <trd>cent</trd>,
    <trd>metr&aacute;k</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_k;&c_v;&c_ib;&c_n;&c_t;&c_a;&c_l; (&c_m;&c_ib;&c_r;&c_a; &c_v;&c_a;&c_g;&c_i;)</trd>,
    <trd>&c_c;&c_je;&c_n;&c_t;&c_n;&c_je;&c_r;</trd>
  </trdgrp>
</drv>
</art>

