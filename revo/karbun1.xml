<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>karbunkol</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="karbun1.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <dif>
      Helru&gcirc;a juvel&scirc;tono, ekz-e rubeno a&ubreve; grenato.
    </dif>
    <refgrp tip="vid">
      Ne konfuzu kun
      <ref cel="karbun.0o">karbunklo</ref>.
    </refgrp>
    <trd lng="be">&c_k;&c_a;&c_r;&c_b;&c_u;&c_n;&c_k;&c_u;&c_l; <klr>(&c_k;&c_a;&c_sh;&c_t;&c_o;&c_w;&c_n;&c_y; &c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;)</klr></trd>
    <trdgrp lng="de">
      <trd>Karfunkel</trd>,
      <trd>Karfunkelstein</trd>
    </trdgrp>
    <trd lng="fr">escarboucle</trd>
    <trd lng="hu">karbunkulus <klr tip="ind">(dr&aacute;gak&odblac;)</klr></trd>
    <trd lng="nl">karbonkel</trd>
    <trd lng="ru">&c_k;&c_a;&c_r;&c_b;&c_u;&c_n;&c_k;&c_u;&c_l; <klr>(&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;)</klr></trd>
  </snc>
</drv>
</art>
