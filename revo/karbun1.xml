<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: karbun1.xml,v 1.16 2022/03/21 17:51:57 revo Exp $">
<kap>
  <rad>karbunkol</rad>/o
  <fnt><bib>ElektFab</bib></fnt>
</kap>
<drv mrk="karbun1.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="karbun1.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Hele ru&gcirc;a <ref tip="super" cel="sxton.juvel0o.MIN">juvel&scirc;tono</ref>, ekzemple rubeno a&ubreve;
      grenato:
      <ekz>
        &scirc;i havis sur la kapo grandegan oran kronon, ornamitan per
        briliantoj kaj <tld/>oj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fi&scirc;kaptisto kaj lia edzino</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_r;&c_b;&c_u;&c_n;&c_k;&c_u;&c_l; <klr>(&c_k;&c_a;&c_sh;&c_t;&c_o;&c_w;&c_n;&c_y;
    &c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;)</klr></trd>
    <trd lng="en">carbuncle <klr tip="amb">(gem)</klr></trd>
    <trdgrp lng="de">
      <trd>Karfunkel</trd>,
      <trd>Karfunkelstein</trd>
    </trdgrp>
    <trd lng="fr">escarboucle</trd>
    <trd lng="hu">karbunkulus <klr tip="ind">(dr&aacute;gak&odblac;)</klr></trd>
    <trd lng="nl">karbonkel</trd>
    <trd lng="ru">&c_k;&c_a;&c_r;&c_b;&c_u;&c_n;&c_k;&c_u;&c_l; <klr>(&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;)</klr></trd>
  </snc>
  <rim>
    Ne konfuzu kun
    <ref cel="karbun.0o">karbunklo</ref>.
  </rim>
  <trd lng="cs">karbunkul</trd>
  <trd lng="pl">karbunku&lstroke; <klr>(dawn. rubin, granat)</klr></trd>     
  <trd lng="ro">buboi</trd>
  <trd lng="sk">karbunkul</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x430;&#x440;&#x431;&#x443;&#x43D;&#x43A;&#x443;&#x43B;</trd>,
    <trd>&#x447;&#x435;&#x440;&#x432;&#x43E;&#x43D;&#x438;&#x439; &#x491;&#x440;&#x430;&#x43D;&#x430;&#x442;</trd>
  </trdgrp>
</drv>
</art>
