<kap>
  <ofc>2</ofc>
  <rad>nikotin</rad>/o
</kap>
<drv mrk="nikoti1.0o">
  <kap><tld/>o</kap>
  <snc mrk="nikoti1.0o.KEM">
    <dif>
      Venena alkaloido eltirata el <ref tip="vid"
      cel="tabak.0o">tabako</ref>:
      <ekz>
        tri tagojn ni havis malbonan man&gcirc;on kaj fumuloj suferis
        pro <tld/>omanko
        <fnt>
        <aut>&Zacute;. &Lstroke;apcionak</aut>,
        <vrk>Esperantisto arestita</vrk>,
        <lok><url 
        ref="http://groups.google.be/group/soc.culture.esperanto?hl=eo">
        soc.culture.esperanto</url>,
        2006-04-10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_ib;&c_k;&c_a;&c_t;&c_y;&c_n;</trd>
  <trd lng="br">nikotin</trd>
  <trdgrp lng="cs">
    <trd>alkaloid tabáku</trd>,
    <trd>nikotin</trd>
  </trdgrp>
  <trd lng="de">Nikotin</trd>
  <trd lng="el">&ny;&jota;&kappa;&omikron;&tau;&jota_ton;&ny;&eta;</trd>
  <trd lng="en">nicotine</trd>
  <trd lng="es">nicotina</trd>
  <trd lng="fr">nicotine</trd>
  <trd lng="hu">nikotin</trd>
  <trd lng="nl">nicotine</trd>
  <trd lng="pl">nikotyna</trd>
  <trd lng="pt">nicotina</trd>
  <trd lng="ru">&c_n;&c_i;&c_k;&c_o;&c_t;&c_i;&c_n;</trd>
  <trd lng="sk">nikotín</trd>
  <trd lng="sv">nikotin</trd>
</drv>
</art>
