</drv>

<drv mrk="nask.0igxi">
  <kap><ofc>*</ofc><tld/>i&gcirc;i <fnt><bib>F</bib>, <lok>Universala
  Vortaro, <url
  ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5117">nask'i&gcirc;'</url></lok></fnt></kap>
  <snc mrk="nask.0igxi.eligxi">
    <dif>
      Esti <tld/>ita, veni en la vivon, aperi en la mondon
      (p.p. ido):
      <ekz>
        <tld/>i&gcirc;is mia kara knabo, kiu &ccirc;i tie dormas
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        tria infano de <nom>Zamenhof</nom> <tld/>i&gcirc;is en 1904
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;a lando
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[deksesjara]</klr>, lia tago de <tld/>i&gcirc;o &gcirc;uste nun estis
        festata
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">geboren werden</trd>
    <trd lng="hu">sz&uuml;letik</trd>
    <trd lng="nl">geboren worden</trd>
    <trd lng="pl">urodzi&cacute; si&eogonek;</trd>
    <trd lng="ru">&c_r;&c_o;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
  <snc mrk="nask.0igxi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ekesti, esti&gcirc;i, okazi (p.p. io):
      <ekz>
        <tld/>i&gcirc;is en li malbona intenco
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 37:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;is en li stranga sento
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;is en &scirc;i fortega deziro foriri, foriri tuj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        en nia tendaro subite <tld/>i&gcirc;is tia granda vento, kiu en la
        da&ubreve;ro de momento minacis <klr>[&#8230;]</klr> al ni
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en Dresden
          en la 17a de a&ubreve;gusto 1908</vrk>
        </fnt>;
      </ekz>
      <ekz>
        li suferis konstante doloron de kapo <klr>[&#8230;]</klr> 
         <klr>[tiu]</klr> doloro <tld/>i&gcirc;is <klr>[&#8230;]</klr> &ccirc;ar 
         li trinkadis &ccirc;iam grandan kvanton da <klr>[&#8230;]</klr> 
         vino en la nokto
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        kion mi nun skribas, <tld/>i&gcirc;is en mia kapo ne nun, sed anta&ubreve;
        kvardek jaroj
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        pro la politika krizo <tld/>i&gcirc;is diversaj malbonaj kutimoj
        <fnt>
          <bib>Monato</bib>
          <aut>Fidilalao Henriel</aut>
          <vrk><url ref="&Monato;/2012/010816.php">Restas
          nur espero</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ili devis batali kontra&ubreve; &ccirc;iam re<tld/>i&gcirc;antaj
        (refoji&gcirc;antaj) duboj, malhelpa&jcirc;oj;
      </ekz><ekz>
        &ccirc;e la <tld/>i&gcirc;o de la tago.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>sz&uuml;letik</trd>,
      <trd>t&aacute;mad <klr tip="ind">(l&eacute;trej&ouml;n)</klr></trd>,
      <trd>keletkezik</trd>
    </trdgrp>
    <trd lng="nl">ontstaan</trd>
    <trd lng="ru">&c_z;&c_a;&c_r;&c_o;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_r;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_r;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="br">beza&ntilde; <ind>ganet</ind></trd>
  <trd lng="de"><ind>geboren</ind> werden</trd>
  <trd lng="es">nacer</trd>
  <trd lng="fr">naitre</trd>
  <trd lng="pl">urodzi&cacute; si&eogonek;</trd>
  <trd lng="pt">nascer</trd>
  <trd lng="ro">a se na&scedil;te</trd>
</drv>

<drv mrk="nask.0ito">
  <kap><tld/>ito</kap>
  <snc mrk="nask.0ito.ido">
    <dif>
      <ref tip="dif" cel="id.0o.GEN">Ido <sncref/></ref>:
      <ekz>
        la&ubreve; la konsilo de mia sinjoro <klr>[...]</klr> ni forigos
        &ccirc;iujn virinojn kaj iliajn <tld/>itojn
        <fnt><bib>MT</bib>, <lok>&Ezr; 10:3</lok></fnt>;
      </ekz><ekz>
        inter <tld/>itoj de virinoj estas neniu pli granda ol Johano
        <fnt><bib>NT</bib>, <lok>&Luk; 7:28</lok></fnt>;
      </ekz>
      <ekz>
        kia <tld/>into, tiaj <tld/>itoj
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>d&iacute;t&ecaron;</trd>,
    <trd>zrozenec</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kind</trd>,
    <trd>Nachkomme</trd>
  </trdgrp>
  <trd lng="es">nacido</trd>
  <trd lng="nl">geborene</trd>
  <trd lng="ru">&c_r;&c_o;&c_zh;&c_d;&c_jo;&c_n;&c_n;&c_y;&c_j;<klr> (&c_s;&c_u;&c_shch;.)</klr></trd>
  <trdgrp lng="sk">
    <trd>die&tcaron;a</trd>,
    <trd>narodenec</trd>
  </trdgrp>
</drv>

<drv mrk="nask.0itaro">
  <kap><tld/>itaro<fnt><bib>MT</bib></fnt></kap>
