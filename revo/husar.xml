<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: husar.xml,v 1.24 2023/11/15 17:08:02 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>husar</rad>/o
</kap>
<drv mrk="husar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="soldat.0o.militisto">Soldato</ref>
      de malpeza <ref tip="vid" cel="kavale.0o">kavalerio</ref>,
      originanta en 16a jarcento en Hungario kaj kies titolo kaj uniformo
      poste disvasti&gcirc;is en multajn e&ubreve;ropajn armeojn:
      <ekz>
        <nom>Walter de Stetten</nom>, oficiro <tld/>a, estis la fian&ccirc;o de <nom>Mathilde</nom>, kaj pro tio li
        estis anka&ubreve; la konfidulo de la du amikinoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        lia surtuto estas ornamita per ar&gcirc;enta broda&jcirc;o, li portas belan <tld/>an
        uniformon, mantelo el nigra veluro &scirc;vebas super la &ccirc;evalo
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_s;&c_a;&c_r;</trd>
  <trd lng="br">houzard</trd>
  <trd lng="cs">husar</trd>
  <trd lng="de">Husar</trd>
  <trdgrp lng="en">
    <trd>hussar</trd>,
    <trd>light cavalry soldier</trd>
  </trdgrp>
  <trd lng="fr">hussard</trd>
  <trd lng="hu">husz&aacute;r</trd>
  <trd lng="it">ussaro</trd>
  <trd lng="ja">&#x8EFD;&#x9A0E;&#x5175;  <pr>&#x3051;&#x3044;&#x304D;&#x3078;&#x3044;</pr></trd>
  <trd lng="nl">huzaar</trd>
  <trd lng="pl">huzar</trd>
  <trd lng="pt">hussardo</trd>
  <trd lng="ru">&c_g;&c_u;&c_s;&c_a;&c_r;</trd>
  <trd lng="sk">hus&aacute;r</trd>
  <trd lng="sv">husar</trd>
  <trdgrp lng="uk">
    <trd>&#x433;&#x443;&#x441;&#x430;&#x440;</trd>,
    <trd>&#x433;&#x443;&#x441;&#x430;&#x440;&#x438;&#x43D;</trd>
  </trdgrp>
  <trd lng="zh">&#x8F15;&#x9A0E;&#x5175; <pr>q&#x12B;ngq&#xED;b&#x12B;ng</pr></trd>
</drv>
</art>
