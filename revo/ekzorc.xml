<vortaro>
<art mrk="$Id$">
<kap>
<rad>ekzorc</rad>(iz)i</kap>
<drv mrk="ekzorc.0">
<kap><tld/>(iz)i</kap>
<gra><vspec>tr</vspec></gra>
<uzo tip="fak">KAT</uzo>
<snc>
<dif>
Pre&gcirc;i por forpeli la diablon. 
</dif>
</snc>
  <trdgrp lng="de">
     <trd>exorzieren</trd>,
     <trd>austreiben</trd>
  </trdgrp>
<trd lng="fr">exorciser</trd>
<trd lng="ru">&c_i;&c_z;&c_g;&c_o;&c_n;&c_ja;&c_t;&c_mol; <klr>(&c_d;&c_mol;&c_ja;&c_v;&c_o;&c_l;&c_a;, &c_d;&c_je;&c_m;&c_o;&c_n;&c_a;, &c_z;&c_l;&c_o;&c_g;&c_o; &c_d;&c_u;&c_h;&c_a;)</klr></trd>
</drv>
<drv mrk="ekzorc.0ajxo">
<kap><tld/>a&jcirc;o</kap>
<snc>
<dif>
Tiucela pre&gcirc;o.  
</dif>
</snc>
  <trdgrp lng="de">
     <trd>Exorzismus</trd>,
     <trd>Teufelsaustreibung</trd>,
     <trd>Teufelsbeschw&ouml;rung</trd>
  </trdgrp>
<trd lng="fr">exorcisme</trd>
</drv>
</art>
