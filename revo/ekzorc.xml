<art mrk="$Id$">
<kap>
<rad>ekzorc</rad>(iz)i</kap>
<drv mrk="ekzorc.0">
<kap><tld/>(iz)i</kap>
<gra><vspec>tr</vspec></gra>
<uzo tip="fak">KAT</uzo>
<snc>
<dif>
Pre&gcirc;i por forpeli la diablon. 
</dif>
</snc>
  <trd lng="be">&c_v;&c_y;&c_g;&c_a;&c_n;&c_ja;&c_c;&c_mol; <klr>(&c_d;'&c_ja;&c_b;&c_l;&c_a;, &c_d;&c_e;&c_m;&c_a;&c_n;&c_a;)</klr></trd>
  <trdgrp lng="de">
     <trd>exorzieren</trd>,
     <trd>austreiben</trd>
  </trdgrp>
  <trd lng="es">exorcisar</trd>
  <trd lng="fr">exorciser</trd>
  <trdgrp lng="hu">
     <trd>&ouml;rd&ouml;g&ouml;t &udblac;z</trd>,
     <trd>ekzorciz&aacute;l</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_z;&c_g;&c_o;&c_n;&c_ja;&c_t;&c_mol; <klr>(&c_d;&c_mol;&c_ja;&c_v;&c_o;&c_l;&c_a;, &c_d;&c_je;&c_m;&c_o;&c_n;&c_a;, &c_z;&c_l;&c_o;&c_g;&c_o; &c_d;&c_u;&c_h;&c_a;)</klr></trd>
</drv>
<drv mrk="ekzorc.0ajxo">
<kap><tld/>a&jcirc;o</kap>
<snc>
<dif>
Tiucela pre&gcirc;o.  
</dif>
</snc>
  <trdgrp lng="de">
     <trd>Exorzismus</trd>,
     <trd>Teufelsaustreibung</trd>,
     <trd>Teufelsbeschw&ouml;rung</trd>
  </trdgrp>
  <trd lng="es">exorcismo</trd>
  <trd lng="fr">exorcisme</trd>
  <trd lng="hu">&ouml;rd&ouml;g&udblac;z&eacute;s</trd>
</drv>
</art>
