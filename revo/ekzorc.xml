<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ekzorc</rad>i
</kap>
<drv mrk="ekzorc.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ekzorc.0i.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Pre&gcirc;i por forpeli demonon a&ubreve; diablon, kiu turmentas iun.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_g;&c_a;&c_n;&c_ja;&c_c;&c_mol; <klr>(&c_d;'&c_ja;&c_b;&c_l;&c_a;, &c_d;&c_e;&c_m;&c_a;&c_n;&c_a;)</klr></trd>
  <trdgrp lng="de">
    <trd>exorzieren</trd>,
    <trd>austreiben</trd>
  </trdgrp>
  <trd lng="es">exorcizar</trd>
  <trd lng="fr">exorciser</trd>
  <trdgrp lng="hu">
    <trd>&ouml;rd&ouml;g&ouml;t &udblac;z</trd>,
    <trd>ekzorciz&aacute;l</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_z;&c_g;&c_o;&c_n;&c_ja;&c_t;&c_mol; <klr>(&c_d;&c_mol;&c_ja;&c_v;&c_o;&c_l;&c_a;, &c_d;&c_je;&c_m;&c_o;&c_n;&c_a;, &c_z;&c_l;&c_o;&c_g;&c_o;
  &c_d;&c_u;&c_h;&c_a;)</klr></trd>
</drv>

<drv mrk="ekzorc.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="ekzorc.0ajxo.REL">
    <dif>
      Pre&gcirc;o uzata por forpeli demonon, diablon.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Exorzismus</trd>,
    <trd>Teufelsaustreibung</trd>,
    <trd>Teufelsbeschw&ouml;rung</trd>
  </trdgrp>
  <trd lng="es">exorcismo</trd>
  <trd lng="fr">exorcisme</trd>
  <trd lng="hu">&ouml;rd&ouml;g&udblac;z&eacute;s</trd>
</drv>

<drv mrk="ekzorc.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="ekzorc.0isto.REL">
    <dif>
      Homo kiu forpelas demonojn el homoj a&ubreve; el ilia medio per pre&gcirc;oj:
      <ekz>
        kelkaj el la vagemaj Judoj, <tld/>istoj, entreprenis nomi
        super tiuj, kiuj havis la malbonajn spiritojn, la nomon de la
        Sinjoro Jesuo
        <fnt><bib>NT</bib>, <lok>&Ago; 19:13</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">exorciste</trd>
</drv>
</art>
