<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ekzorc</rad>i
</kap>

<drv mrk="ekzorc.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ekzorc.0i.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Pre&gcirc;i por forpeli demonon, diablon, misa&jcirc;on, kiu
      turmentas iun:
      <ekz>
        li <tld/>os diablon
        <fnt><bib>Kiso</bib>, <lok>la bovinoj de la
        paro&hcirc;estro</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_g;&c_a;&c_n;&c_ja;&c_c;&c_mol;
  <klr>(&c_d;'&c_ja;&c_b;&c_l;&c_a;,
  &c_d;&c_e;&c_m;&c_a;&c_n;&c_a;)</klr></trd>
  <trd lng="bg">&c_je;&c_k;&c_z;&c_o;&c_r;&c_s;&c_i;&c_s;&c_t;</trd>
  <trd lng="ca">exorcitzar</trd>
  <trdgrp lng="de">
    <trd>exorzieren</trd>,
    <trd>austreiben</trd>
  </trdgrp>
  <trd lng="el">&xi;&omikron;&rho;&kappa;&jota_ton;&zeta;&omega;</trd>
  <trd lng="es">exorcizar</trd>
  <trd lng="fr">exorciser</trd>
  <trdgrp lng="hu">
    <trd>&ouml;rd&ouml;g&ouml;t &udblac;z</trd>,
    <trd>ekzorciz&aacute;l</trd>
  </trdgrp>
  <trd lng="pl">odprawia&cacute; <ind>egzorcyzmy</ind></trd>
  <trd lng="ru">&c_i;&c_z;&c_g;&c_o;&c_n;&c_ja;&c_t;&c_mol;
  <klr>(&c_d;&c_mol;&c_ja;&c_v;&c_o;&c_l;&c_a;,
  &c_d;&c_je;&c_m;&c_o;&c_n;&c_a;, &c_z;&c_l;&c_o;&c_g;&c_o;
  &c_d;&c_u;&c_h;&c_a;)</klr></trd>
</drv>

<drv mrk="ekzorc.0o">
  <kap><tld/>o</kap>
  <snc mrk="ekzorc.0o.ekzorcado">
    <dif>
      <tld lit="E"/>ado.
    </dif>
  </snc>
  <snc mrk="ekzorc.0o.ekzorcajxo">
    <ref tip="dif" cel="ekzorc.0ajxo.REL"><tld/>a&jcirc;o</ref>
    <ekz>
      antikva <tld/>o celanta forpeli malsanon
      <fnt>
      <aut>Pang Bian</aut>,
      <vrk>Moroj pri la Printempa Festo...</vrk>,
      <lok><url
      ref="http://japanese.china.org.cn/EL/EL/ElPopolaCxinio/2000-02/bd2000-2.htm">El
      popola &Ccirc;inio, 2000-02</url></lok>
      </fnt>
    </ekz>
  </snc>
  <trd lng="ca">exorcisme<klr> (ritu)</klr></trd>
  <trd lng="fr">exorcisme</trd>
  <trd lng="pl">egzorcyzm</trd>
</drv>

<drv mrk="ekzorc.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="ekzorc.0ajxo.REL">
    <dif>
      Pre&gcirc;o uzata por forpeli demonon, diablon.
    </dif>
  </snc>
  <trd lng="ca">exorcisme<klr> (preg&agrave;ria)</klr></trd>
  <trdgrp lng="de">
    <trd>Exorzismus</trd>,
    <trd>Teufelsaustreibung</trd>,
    <trd>Teufelsbeschw&ouml;rung</trd>
  </trdgrp>
  <trd lng="es">exorcismo</trd>
  <trd lng="fr">exorcisme</trd>
  <trd lng="hu">&ouml;rd&ouml;g&udblac;z&eacute;s</trd>
  <trd lng="pl">egzorcyzm</trd>
</drv>

<drv mrk="ekzorc.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="ekzorc.0isto.REL">
    <dif>
      Homo kiu forpelas demonojn el homoj a&ubreve; el ilia medio per
      pre&gcirc;oj:
      <ekz>
        kelkaj el la vagemaj Judoj, <tld/>istoj, entreprenis nomi
        super tiuj, kiuj havis la malbonajn spiritojn, la nomon de la
        Sinjoro Jesuo
        <fnt><bib>NT</bib>, <lok>&Ago; 19:13</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">exorcista</trd>
  <trd lng="de">Exorzist</trd>
  <trd lng="fr">exorciste</trd>
  <trd lng="pl">egzorcysta</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_k;&c_z;&c_o;&c_r;&c_c;&c_i;&c_s;&c_t;</trd>,
    <trd>&c_i;&c_z;&c_g;&c_o;&c_n;&c_ja;&c_ju;&c_shch;&c_i;&c_j;
    &c_b;&c_je;&c_s;&c_a;</trd>
  </trdgrp>
</drv>
</art>
