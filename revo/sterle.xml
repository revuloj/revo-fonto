<art mrk="$Id: sterle.xml,v 1.17 2023/10/22 21:20:27 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sterled</rad>/o
</kap>
<drv mrk="sterle.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_specioj" cel="speci.0o">Specio</ref>
      de <ref tip="lst" cel="acipenser.0o" lst="voko:fi&scirc;oj">acipensero</ref>
      (<trd lng="la">Acipenser ruthenus</trd>), kun malgranda mallar&gcirc;a
      korpo &gcirc;is maksimume 1&#x202F;m longa, kun kapo pli longa kaj maldika ol <ref
      cel="sturg.0o">sturgo</ref>:
      <ekz>
        <tld/>oj lo&gcirc;as en la basenoj de la riveroj de la Nigra maro
        kaj la Kaspia maro, de <klr>[&#8230;]</klr> Obo, Jenisejo <klr>[&#8230;]</klr>,
        penetris en la basenojn de la Ladoga kaj Onega lagoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=407509">Sterledo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
    lok="&WCU;/thumb/3/3d/Acipenser_ruthenus_Prague_Vltava_1.jpg/640px-Acipenser_ruthenus_Prague_Vltava_1.jpg"
    prm="PD"><tld/>o en ekspozicio en Prago 2011, &Ccirc;e&hcirc;ujo
        <fnt>
          <aut>Karelj, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Acipenser_ruthenus_Prague_Vltava_1.jpg">&WCW;/File:Acipenser_ruthenus_Prague_Vltava_1.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_c;&c_je;&c_r;&c_l;&c_ja;&c_d;&c_z;&c_mol;</trd>
  <trd lng="cs">jeseter mal&yacute;</trd>
  <trd lng="de">Sterlet</trd>
  <trd lng="fr">esturgeon sterlet</trd>
  <trd lng="hu">kecsege</trd>
  <trd lng="ja">&#x30B3;&#x30C1;&#x30E7;&#x30A6;&#x30B6;&#x30E1;</trd>
  <trd lng="nl">sterlet</trd>
  <trdgrp lng="pl">
    <trd>sterlet</trd>,
    <trd>czeczuga</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_r;&c_l;&c_ja;&c_d;&c_mol;</trd>
  <trd lng="sk">jeseter mal&yacute;</trd>
  <trd lng="uk">&c_s;&c_t;&c_je;&c_r;&#x43B;&#x44F;&c_d;&#x44C;</trd>
</drv>
</art>
