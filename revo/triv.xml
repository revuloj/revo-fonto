<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: triv.xml,v 1.9 2022/03/28 14:44:12 revo Exp $">
<kap>
    <rad>triv</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="triv.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="triv.0i.KOMUNE">
    <dif>
      <ref tip="dif" cel="uz.el0i.foruzi">Eluzi<sncref/></ref>:
      <ekz>
        sub ni estas <tld/>ita kaj malpura littuko
        <fnt><aut>B. Vaha</aut>, <vrk>Adolesko</vrk>, 
        <lok>1987</lok></fnt>;
      </ekz>
      <ekz>
        la mort' ne estus tamen fin', /
        ni plu memore vivus, /
        &ccirc;ar nur forges' mortigus nin /
        kaj nian vivon <tld/>us
        <fnt>
          <aut>Cezar</aut>,
          <vrk><url ref="http://www.cezarpoemoj.cezarpoezio.de/199.html">
          Vivo kaj amo</url></vrk>,
          <lok>[vidita en 2010]</lok>
        </fnt>;
      </ekz>
      <ekz>
        viziti Toskanion for de la <tld/>itaj itineroj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2012/010845.php">Saturnia: Vintra
          magio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        19a jarcento, <tld/>itraj T-formaj fenestroj
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2014/011095.php">Tammisaari, urbeto
          &ccirc;e Balta Maro</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        alta orelfotelo el <tld/>ita ledo
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Sen&ccirc;esa Rakonto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la glitlagroj eltenas konsiderindajn frapojn kaj super&scirc;ar&gcirc;on,
        interspacon, kiu ekestas per <tld/>o oni povas facile elimini
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=134527">Lagro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>abnutzen</trd>,
      <trd>verschlei&szlig;en</trd>,
      <trd>ausleiern</trd>
    </trdgrp>
  </snc>
  <trd lng="ca">desgastar</trd>
  <trdgrp lng="cs">
    <trd>obnosit</trd>,
    <trd>opot&rcaron;ebovat</trd>
  </trdgrp>
  <trd lng="es">desgastar</trd>
  <trd lng="fr">user</trd>
  <trdgrp lng="it">
    <trd>consumare</trd>,
    <trd>logorare</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dra&tcaron;</trd>,
    <trd>obnosi&tcaron;</trd>,
    <trd>o&scaron;&uacute;cha&tcaron;</trd>,
    <trd>zodra&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x437;&#x43D;&#x43E;&#x448;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x440;&#x43E;&#x442;&#x438;&#x440;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x438;&#x441;&#x43D;&#x430;&#x436;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x437;&#x43D;&#x435;&#x441;&#x438;&#x43B;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x438;&#x43C;&#x43E;&#x442;&#x443;&#x432;&#x430;&#x442;&#x438; &#x441;&#x438;&#x43B;&#x438; (&#x441;&#x443;&#x43F;&#x440;&#x43E;&#x442;&#x438;&#x432;&#x43D;&#x438;&#x43A;&#x430;)</trd>
  </trdgrp>
</drv>
</art>

