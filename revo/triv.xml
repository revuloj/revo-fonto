<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: triv.xml,v 1.12 2024/01/25 18:30:25 revo Exp $">
<kap>
    <rad>triv</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="triv.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="triv.0i.KOMUNE">
    <dif>
      <ref tip="dif" cel="uz.el0i.foruzi">Eluzi<sncref/></ref>:
      <ekz>
        sub ni estas <tld/>ita kaj malpura littuko
        <fnt><aut>B. Vaha</aut>, <vrk>Adolesko</vrk>,
        <lok>1987</lok></fnt>;
      </ekz>
      <ekz>
        la mort' ne estus tamen fin', /
        ni plu memore vivus, /
        &ccirc;ar nur forges' mortigus nin /
        kaj nian vivon <tld/>us
        <fnt>
          <aut>Cezar</aut>,
          <vrk><url ref="http://www.cezarpoemoj.cezarpoezio.de/199.html">
          Vivo kaj amo</url></vrk>,
          <lok>[vidita en 2010]</lok>
        </fnt>;
      </ekz>
      <ekz>
        viziti Toskanion for de la <tld/>itaj itineroj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2012/010845.php">Saturnia: Vintra
          magio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        alta orelfotelo el <tld/>ita ledo
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Sen&ccirc;esa Rakonto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la glitlagroj eltenas konsiderindajn frapojn kaj super&scirc;ar&gcirc;on,
        interspacon, kiu ekestas per <tld/>o oni povas facile elimini
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=134527">Lagro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>abnutzen</trd>,
      <trd>verschlei&szlig;en</trd>,
      <trd>ausleiern</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_n;&c_o;&c_sh;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_a;&c_d;&c_z;&c_je;&c_n;&c_mol;&c_n;&c_je;)</klr></trd>,
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_b;&c_ib;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_a;&c_b;&c_u;&c_t;&c_a;&c_k;, &c_d;&c_a;&c_r;&c_o;&c_g;&c_u; &c_ib; &c_d;&c_a; &c_g;. &c_p;.)</klr></trd>
  </trdgrp>
  <trd lng="ca">desgastar</trd>
  <trdgrp lng="cs">
    <trd>obnosit</trd>,
    <trd>opot&rcaron;ebovat</trd>
  </trdgrp>
  <trd lng="es">desgastar</trd>
  <trd lng="fr">user</trd>
  <trdgrp lng="it">
    <trd>consumare</trd>,
    <trd>logorare</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wyciera&cacute;</trd>,
    <trd>zdziera&cacute;</trd>,
    <trd>zetrze&cacute;</trd>,
    <trd>zu&zdot;ywa&cacute;  <klr>(przez tarcie)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dra&tcaron;</trd>,
    <trd>obnosi&tcaron;</trd>,
    <trd>o&scaron;&uacute;cha&tcaron;</trd>,
    <trd>zodra&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&#x43D;&#x43E;&c_sh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_r;&#x43E;&c_t;&c_i;&c_r;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_s;&#x43D;&c_a;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&#x43D;&c_je;&c_s;&c_i;&#x43B;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&#x43C;&#x43E;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i; &c_s;&c_i;&#x43B;&c_i; (&c_s;&c_u;&#x43F;&c_r;&#x43E;&c_t;&c_i;&c_v;&#x43D;&c_i;&#x43A;&c_a;)</trd>
  </trdgrp>
</drv>
</art>

