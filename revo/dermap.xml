<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dermap.xml,v 1.11 2022/03/15 15:50:55 revo Exp $">
<kap>
<rad>dermapter</rad>/oj
</kap>
<drv mrk="dermap.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_ordoj" cel="ord.0o.BIO">Ordo</ref>
      el la klaso <ref tip="malprt" cel="insekt.0oj">insektoj</ref>
      (<trd lng="la">Dermaptera</trd>):
      <ekz>
        ovoj de verdaj <tld/>oj estas bone pendigitaj unuope sur longa filamenta tigo,
        supozeble por malhelpi predadon fare de insektoj kiel koleopteroj
        <fnt>
          <aut>Ralph A. Lewin</aut>
          <vrk><url ref="https://scienca-revuo.info/article/viewFile/1601/1580">Ovoj</url></vrk>
        </fnt>.
      </ekz>      
    </dif>
    <ref tip="prt" cel="forfik.0o">forfikulo</ref>
  </snc>
  <trd lng="ca">derm&agrave;pters</trd>
  <trd lng="de">Ohrw&uuml;rmer <klr tip="ind">(Ordnung)</klr></trd>
  <trd lng="fr">dermapt&egrave;res</trd>
  <trd lng="hu">f&uuml;lbem&aacute;sz&oacute;k</trd>
  <trd lng="pl">skorki</trd>
  <trd lng="pt">derm&aacute;pteros</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_zh;&c_i;&c_s;&c_t;&c_o;&c_k;&c_r;&c_y;&c_l;&c_y;&c_je;<klr tip="ind"> (&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>,
    <trd>&c_u;&c_h;&c_o;&c_v;&c_jo;&c_r;&c_t;&c_k;&c_i; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x448;&#x43A;&#x456;&#x440;&#x44F;&#x441;&#x442;&#x43E;&#x43A;&#x440;&#x438;&#x43B;&#x456;</trd>,
    <trd>&#x449;&#x438;&#x43F;&#x430;&#x432;&#x43A;&#x438;</trd>
  </trdgrp>
</drv>
</art>
