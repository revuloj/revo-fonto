<kap>
  <ofc>9</ofc>
  <rad>brokol</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="brokol.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Italdevena <ref tip="super" cel="brasik.flor0o">florbrasiko</ref>
      (<trd lng="la">Brassica oleracea var. botrytis italica</trd>), kun
      verda malpli densa kapo:
      <ekz>
        man&gcirc;ante laktukon a&ubreve; <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van De Velde</aut>
          <vrk><url ref="&Monato;/2013/010934.php">Sablohorlo&gcirc;o
          for&scirc;ovas piramidon</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        unu kilogramo da <tld/>o monate malpliigas la riskon de
        mamkancero je 40&nbsp;%
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van De Velde</aut>
          <vrk><url ref="&Monato;/2013/010934.php">Sablohorlo&gcirc;o
          for&scirc;ovas piramidon</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_a;&c_k;&c_o;&c_l;&c_ib;</trd>
  <trdgrp lng="de">
    <trd>Brokkoli</trd>,
    <trd>Broccoli</trd>
  </trdgrp>
  <trd lng="en">broccoli</trd>
  <trd lng="es">br&oacute;coli</trd>
  <trd lng="fr">brocoli</trd>
  <trd lng="he">&#x5D1;&#x5E8;&#x5D5;&#x5E7;&#x5D5;&#x5DC;&#x5D9;</trd>
  <trdgrp lng="hu">
    <trd>sp&aacute;rgakel</trd>,
    <trd>brokkoli</trd>
  </trdgrp>
  <trd lng="id">brokoli</trd>
  <trd lng="ja">&#x30D6;&#x30ED;&#x30C3;&#x30B3;&#x30EA;&#x30FC;</trd>
  <trd lng="nl">broccoli</trd>
  <trd lng="pl">broku&lstroke;y</trd>
  <trd lng="pt">br&oacute;colis</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_o;&c_k;&c_k;&c_o;&c_l;&c_i;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_r;&c_zh;&c_je;&c_v;&c_a;&c_ja;
    &c_k;&c_a;&c_p;&c_u;&c_s;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">brokolica</trd>
  <trd lng="sv">broccoli</trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x43F;&#x430;&#x440;&#x436;&#x435;&#x432;&#x430; &#x43A;&#x430;&#x43F;&#x443;&#x441;&#x442;&#x430;</trd>,
    <trd>&#x431;&#x440;&#x43E;&#x43A;&#x43E;&#x43B;&#x456;</trd>
  </trdgrp>
</drv>
