<vortaro>
<art mrk="$Id$">
<kap>
<rad>brokol</rad>/o
</kap>
<drv mrk="brokol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Italdevena <ref tip="super" cel="brasik.flor0o">florbrasiko</ref>
      (<trd lng="la">Brassica oleracea var. botrytis italica</trd>), kun
      verda malpli densa kapo.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_a;&c_k;&c_o;&c_l;&c_ib;</trd>
  <trd lng="de">Broccoli</trd>
  <trd lng="en">broccoli</trd>
  <trd lng="fr">brocoli</trd>
  <trdgrp lng="hu">
    <trd>sp&aacute;rgakel</trd>,
    <trd>brokkoli</trd>
  </trdgrp>
  <trd lng="nl">broccoli</trd>
  <trd lng="pl">broku&lstroke;y</trd>
  <trd lng="pt">br&oacute;colis</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_o;&c_k;&c_k;&c_o;&c_l;&c_i;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_r;&c_zh;&c_je;&c_v;&c_a;&c_ja;
&c_k;&c_a;&c_p;&c_u;&c_s;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">brokolica</trd>
  <trd lng="sv">broccoli</trd>
</drv>
</art>
