<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>glukoz</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="glukoz.0o">
  <kap><tld/>o</kap>
  <snc mrk="glukoz.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <ref tip="dif" cel="glikoz.0o.KEM">glikozo</ref>
      <ekz>
        la korpo nur uzis a&ubreve; grason a&ubreve; proteinon pro manko de <tld/>o 
        en la &ccirc;elo, kaj la paciento maldiki&gcirc;as
        <fnt>
          <aut>I. Cris&oacute;gno</aut>,
          <vrk><url
          ref="http://www.geocities.com/HotSprings/4234/diabeto.html?200824">
          Diabeto</url></vrk>,
          <lok>[1996?]</lok>
        </fnt>.
      </ekz>
  </snc>
  <trd lng="be">&c_g;&c_l;&c_ju;&c_k;&c_o;&c_z;&c_a;</trd>
  <trd lng="bg">&c_g;&c_l;&c_ju;&c_k;&c_o;&c_z;&c_a;</trd>
  <trd lng="ca">glucosa</trd>
  <trdgrp lng="cs">
    <trd>gluk&oacute;za</trd>,
    <trd>hroznov&yacute; cukr</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Glukose</trd>,
    <trd>Glykose</trd>,
    <trd>Traubenzucker</trd>
  </trdgrp>
  <trd lng="en">glucose</trd>
  <trdgrp lng="fa">
    <trd>&f_g;&a_l;&a_w;&f_k;&a_z;</trd>,
    <trd>&a_q;&a_n;&a_d; &a_A;&a_n;&f_g;&a_w;&a_r;</trd>,
    <trd>&a_d;&f_k;&a_s;&a_t;&a_r;&a_w;&a_z;</trd>
  </trdgrp>
  <trd lng="fr">glucose</trd>
  <trdgrp lng="nl">
    <trd>glucose</trd>,
    <trd>dextrose</trd>,
    <trd>druivensuiker</trd>
  </trdgrp>
  <trd lng="pt">glicose</trd>
  <trd lng="ru">&c_g;&c_l;&c_ju;&c_k;&c_o;&c_z;&c_a;</trd>
  <trd lng="sk">gluk&oacute;za</trd>
</drv>
</art>

