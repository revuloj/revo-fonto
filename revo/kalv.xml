<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>kalv</rad>/a
</kap>
<drv mrk="kalv.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Perdinta la kapharojn:
      <ekz>
        kapo malsa&gcirc;a nek grizi&gcirc;as, nek
        <tld/>i&gcirc;as<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="alopec.0o">alopecio</ref>.
    </refgrp>
    <trdgrp lng="en">
      <trd>bald</trd>,
      <trd>bald-headed</trd>,
      <trd>hairless</trd>
    </trdgrp>
    <trd lng="es">calvo</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senvegeta&jcirc;a:
      <ekz>
        <tld/>a monto.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="nud.0a">nuda</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_y;&c_s;&c_y;</trd>       
  <trd lng="br">moal</trd>
  <trd lng="de">kahl</trd>
  <trd lng="fr">chauve</trd>
  <trd lng="hu">kopasz</trd>
  <trd lng="nl">kaal</trd>
  <trd lng="ru">&c_l;&c_y;&c_s;&c_y;&c_j;</trd>
</drv>
</art>
