<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>kalv</rad>/a
</kap>
<drv mrk="kalv.0a">
  <kap><tld/>a</kap>
  <snc num="1">
    <dif>
      Perdinta la kapharojn:
      <ekz>
        kapo malsa&gcirc;a nek grizi&gcirc;as, nek
        <tld/>i&gcirc;as<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="alopec.0o">alopecio</ref>.
    </refgrp>
  <trdgrp lng="en">
    <trd>bald</trd>,
    <trd>bald-headed</trd>,
    <trd>hairless</trd>
  </trdgrp>
  </snc>
  <snc num="2">
    <dif>
      Senvegeta&jcirc;a: <ekz><tld/>a monto</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="nud.0a">nuda</ref>.
    </refgrp>
  </snc>
  <trd lng="de">kahl</trd>
  <trd lng="hu">kopasz</trd>
  <trd lng="nl">kaal</trd>
  <trd lng="ru">&c_l;&c_y;&c_s;&c_y;&c_j;</trd>
</drv>
</art>
