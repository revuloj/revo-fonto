<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>atrakci</rad>/o <fnt><bib>SPIV</bib></fnt>
</kap>
<drv mrk="atrakci.0o">
  <kap><tld/>o</kap>
  <snc mrk="atrakci.0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Unuopa <ref tip="super" cel="spekt.0ajxo">spekta&jcirc;o</ref> en cirko
      a&ubreve;
      en <ref tip="malprt" cel="variet.0o">varieteo</ref>, unuopa ludo
      en
      publika distrejo:
      <ekz>
        la&ubreve;longe de Moskva-rivero eltiris sin, alterne kun
        frukto-vendejoj
        kaj trinkbudoj, <tld/>oj
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_t;&c_r;&c_a;&c_k;&c_c;&c_y;&c_jo;&c_n;</trd>
  <trd lng="de">Attraktion</trd>
  <trd lng="es">atracci&oacute;n <klr> (espect&aacute;culo)</klr></trd>
  <trd lng="fr">attraction<klr> (spectacle)</klr></trd>
  <trdgrp lng="nl">
    <trd>attractie</trd>,
    <trd>vermakelijkheid</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_t;&c_t;&c_r;&c_a;&c_k;&c_c;&c_i;&c_o;&c_n;</trd>
</drv>
</art>

