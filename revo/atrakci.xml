<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: atrakci.xml,v 1.15 2022/03/12 15:33:56 revo Exp $">
<kap>
  <rad>atrakci</rad>/o <fnt><bib>SPIV</bib></fnt>
</kap>
<drv mrk="atrakci.0o">
  <kap><tld/>o</kap>
  <snc mrk="atrakci.0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="super" cel="spekt.0ajxo">Spekta&jcirc;o</ref> en cirko
      a&ubreve; en <ref tip="malprt" cel="variet.0o">varieteo</ref> ks,
      aparte vekanta la intereson de la publiko:
      <ekz>
        la&ubreve;longe de Moskva-rivero eltiris sin, alterne kun
        frukto-vendejoj
        kaj trinkbudoj, <tld/>oj
        <fnt><bib>Metrop</bib></fnt>;
      </ekz>
      <ekz>
        la pinta <tld/>o de la spektaklo
        <fnt>
          <bib>MD17</bib>
          <vrk>Triumfo de la Paranoja Stilo</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="amuz.0ajxo">amuza&jcirc;o</ref>,
       <ref cel="distr.0ajxo">distra&jcirc;o</ref>,
       <ref cel="vid.0indajxo">vidinda&jcirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_t;&c_r;&c_a;&c_k;&c_c;&c_y;&c_jo;&c_n;</trd>
  <trdgrp lng="ca">
    <trd>atracci&oacute;</trd>,
    <trd>espectacle</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>atrakce</trd>,
    <trd>poutav&aacute; z&aacute;bava</trd>
  </trdgrp>
  <trd lng="de">Attraktion</trd>
  <trd lng="en">attraction</trd>
  <trd lng="es">atracci&oacute;n <klr> (espect&aacute;culo)</klr></trd>
  <trd lng="fr">attraction<klr> (spectacle)</klr></trd>
  <trd lng="he">&mem;&#x5B0;&shin;&#x5C1;&#x5B4;&yod;&kaf;&#x5B8;&he;</trd>
  <trd lng="it">attrazione<klr> (spettacolo)</klr></trd>
  <trdgrp lng="nl">
    <trd>attractie</trd>,
    <trd>vermakelijkheid</trd>
  </trdgrp>
  <trd lng="pl">atrakcja</trd>
  <trd lng="ro">atrac&#x021b;ie</trd>
  <trd lng="ru">&c_a;&c_t;&c_t;&c_r;&c_a;&c_k;&c_c;&c_i;&c_o;&c_n;</trd>
  <trd lng="sk">atrakcia</trd>
  <trd lng="uk">&#x430;&#x442;&#x440;&#x430;&#x43A;&#x446;&#x456;&#x43E;&#x43D;</trd>
</drv>
</art>

