<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kinematograf</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kinema1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Aparato, projekcianta sur ekranon fotografajn lumbildojn,
      kies multeco kaj rapideco naskas iluzion de efektiva movo. 
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>film projector</trd>,
    <trd>movie projector</trd>,
    <trd>motion picture projector</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_n;&c_je;&c_m;&c_a;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>,
    <trd>&c_k;&c_i;&c_n;&c_o;&c_p;&c_r;&c_o;&c_je;&c_k;&c_t;&c_o;&c_r;</trd>
  </trdgrp>
</drv>
<drv mrk="kinema1.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Loko, en kiu oni povas vidi <tld/>ajn bildojn; filmteatro. 
    </dif>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_n;&c_o;&c_t;&c_je;&c_a;&c_t;&c_r;</trd>
  </trdgrp>
</drv>
<drv mrk="kinema1.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Filmo. 
    </dif>
  </snc>
  <trd lng="ru">&c_k;&c_i;&c_n;&c_o;&c_f;&c_i;&c_l;&c_mol;&c_m;</trd>
</drv>
<drv mrk="kinema1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Projekcii <tld/>ajn bildojn.  
    </dif>
  </snc>
</drv>
</art>
