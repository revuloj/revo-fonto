<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kinematograf</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kinema1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KIN</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="projek.0ilo">Projekciilo</ref> de filmaj lumbildoj,
      kies multeco kaj rapideco naskas iluzion de efektiva movo.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_n;&c_a;&c_p;&c_r;&c_a;&c_je;&c_k;&c_t;&c_a;&c_r;</trd>
  <trd lng="de">Filmprojektor</trd>
  <trdgrp lng="en">
    <trd>film projector</trd>,
    <trd>movie projector</trd>,
    <trd>motion picture projector</trd>
  </trdgrp>
  <trd lng="fr">cin&eacute;matographe</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_n;&c_je;&c_m;&c_a;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>,
    <trd>&c_k;&c_i;&c_n;&c_o;&c_p;&c_r;&c_o;&c_je;&c_k;&c_t;&c_o;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="kinema1.0ejo">
  <kap><tld/>ejo</kap>
  <uzo tip="fak">KIN</uzo>
  <snc>
    <ref tip="dif" cel="kin.0ejo">kinejo</ref>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_n;&c_a;&c_t;&c_e;&c_a;&c_t;&c_r;</trd>
  <trdgrp lng="de">
    <trd>Kino</trd>,
    <trd>Filmtheater</trd>
  </trdgrp>
  <trd lng="fr">cin&eacute;ma<klr> (lieu)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_n;&c_o;&c_t;&c_je;&c_a;&c_t;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="kinema1.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">KIN</uzo>
  <snc>
    <ref tip="dif" cel="kin.0ajxo">kina&jcirc;o</ref>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_n;&c_a;&c_f;&c_ib;&c_l;&c_mol;&c_m;</trd>
  <trd lng="de">Film</trd>
  <trdgrp lng="fr">
    <trd>film<klr> (oeuvre cin&eacute;matographique)</klr></trd>,
    <trd>oeuvre cin&eacute;matographique</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_i;&c_n;&c_o;&c_f;&c_i;&c_l;&c_mol;&c_m;</trd>
</drv>

<drv mrk="kinema1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">KIN</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="projek.0i.FOT">Projekcii</ref> <tld/>ajn bildojn.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_k;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_k;&c_ib;&c_n;&c_a;&c_f;&c_ib;&c_l;&c_mol;&c_m;)</klr></trd>
  <trd lng="fr">projeter<klr> (un film)</klr></trd>
</drv>
</art>
