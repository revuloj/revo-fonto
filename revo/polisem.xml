<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: polisem.xml,v 1.5 2024/03/11 21:24:09 revo Exp $">
<kap>
  <rad>polisem</rad>/o
</kap>
<drv mrk="polisem.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="polisem.0o.LIN">
    <dif>
      <ref tip="dif" cel="senc.plur0ajxo.LIN">Plursenca&jcirc;o</ref>:
      <ekz>
        ekzistu nek plursignifaj nomoj (homonimoj kaj <tld/>oj),
        nek pluraj nomoj (sinonimoj) por unu nocio
        <fnt>
          <aut>Akademio de Esperanto</aut>,
          <vrk><url
          ref="&AdE;/verkoj/faka_lingvo_raporto20110415.pdf">
          Raporto pri libromanuskripto kun la titolo
          Ekoturisma vortareto</url></vrk>,
          <lok>2011-04-15</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="polisemi.0o">polisemio</ref>
  </snc>
<trdgrp lng="be">
  <trd>&c_sh;&c_m;&c_a;&c_t;&c_z;&c_n;&c_a;&c_ch;&c_n;&c_a;&c_je; <ind>&c_s;&c_l;&c_o;&c_v;&c_a;</ind></trd>,
  <trd>&c_sh;&c_m;&c_a;&c_t;&c_z;&c_n;&c_a;&c_ch;&c_n;&c_y; <ind>&c_v;&c_y;&c_r;&c_a;&c_z;</ind></trd>
</trdgrp>
<trd lng="pl">wyraz wieloznaczny</trd>
</drv>
</art>

