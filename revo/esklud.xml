<vortaro>
<art mrk="$Id: esklud.xml,v 1.4 2010/03/05 17:30:40 revo Exp $">
<kap>
  <rad>esklud</rad>/i
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="esklud.0i">
  <kap><tld/>i</kap>
  <snc mrk="esklud.0i.ekskluzivi">
    <ref tip="dif" cel="eksklu.0i">ekskluzivi</ref>
    <ekz>
      la ludgvidisto havas krome la rajton en motivitaj kazoj <tld/>i
      iun
      ludiston
      <fnt>
      <aut>Tibor Ujlaky-Nagy</aut>,
      <vrk><url ref="http://www.fw.hu/eventoj/steb/sportalingvo.rtf">La
      Sporta Lingvo en Esperanto</url></vrk>,
      <lok>Hungara Esperanto-Asocio, 1972</lok>
      </fnt>.
    </ekz>
    <refgrp tip="vid">
      <ref cel="eks.0igi">eksigi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>ausschlie&szlig;en</trd>,
    <trd>ausnehmen</trd>
  </trdgrp>
  <trd lng="es">excluir</trd>
  <trd lng="fr">exclure</trd>
  <trd lng="hu">kiz&aacute;r<klr tip="ind"> (&aacute;tv.)</klr></trd>
  <trd lng="pl">wyklucza&cacute;</trd>
  <trd lng="pt">excluir</trd>
  <trd lng="ru">&c_i;&c_s;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
