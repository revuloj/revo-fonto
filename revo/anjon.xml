<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: anjon.xml,v 1.10 2024/08/27 15:29:38 revo Exp $">
<kap>
  <rad>anjon</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="anjon.0o">
  <kap><tld/>o</kap>
  <snc mrk="anjon.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="super" cel="jon.0o.KEM">Jono</ref>
      ricevinta negativan elektran &scirc;argon:
      <ekz>
        &ccirc;irka&ubreve; katjono grupi&gcirc;as <tld/>oj kun kontra&ubreve;a &scirc;argo
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://eo.wikibooks.org/wiki/Kondukilo_en_kemion">
          Kondukilo en kemion</url></vrk>,
          <lok>Vikilibroj, 2013-10-13</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="katjon.0o.KEM">katjono</ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#38452;&#x79BB;&#x5B50; <pr>y&#x12B;nl&#xED;z&#x1D0;</pr></trd>,
    <trd>&#38512;&#x96E2;&#x5B50; <pr>y&#x12B;nl&#xED;z&#x1D0;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_n;&c_ib;&c_jo;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>anion</trd>,
    <trd>aniont</trd>,
    <trd>z&aacute;porn&yacute; ion</trd>
  </trdgrp>
  <trd lng="de">Anion</trd>
  <trd lng="en">anion</trd>
  <trd lng="fr">anion</trd>
  <trd lng="ja">&#38512;&#x30A4;&#x30AA;&#x30F3;  <pr>&#12356;&#12435;&#x30A4;&#x30AA;&#x30F3;</pr>
</trd>
  <trdgrp lng="pl">
    <trd>jon ujemny</trd>,
    <trd>anion</trd>
  </trdgrp>
  <trd lng="sk">ani&oacute;n</trd>
  <trd lng="uk">&c_a;&#x43D;&c_ib;&#x43E;&#x43D;</trd>
</drv>
</art>

