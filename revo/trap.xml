<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: trap.xml,v 1.17 2022/05/05 17:08:11 revo Exp $">
<kap>
   <rad>trap</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="trap.0o">
   <kap><tld/>o</kap>
   <snc mrk="trap.0o.planto">
     <uzo tip="fak">BOT</uzo>
     <dif>
       <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref> de longaj 
       flosaj vegeta&jcirc;oj (<trd lng="la">Trapa</trd>)
       kun rombaj, dikaj folioj, blankaj floretoj, prosperaj en
       duonvarma, iom stagna akvo en Azio, E&ubreve;ropo, Afriko kaj pro homa enkonduko
       en Nordameriko kaj A&ubreve;stralio:
       <ekz>
         kiu povas flari bonan odoron el <tld/>a floro?
         <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 80a, volumo 2a, p.
         670a</lok></fnt>
       </ekz>
     </dif>
     <trdgrp lng="fr">
       <trd>macle</trd>,
       <trd>macre</trd>
     </trdgrp>
     <trd lng="hu">sulyom</trd>
     <trd lng="de">Wassernuss<klr>pflanze</klr></trd>
     <trdgrp lng="pl">
       <trd>kotewka</trd>,
       <trd>orzech wodny</trd>,
       <trd><ind>kotewka</ind> orzech wodny</trd>
     </trdgrp>
     <trd lng="pt">trapa</trd>
     <trdgrp lng="ru">
       <trd>&c_v;&c_o;&c_d;&c_ja;&c_n;&c_o;&c_j; &c_o;&c_r;&c_je;&c_h;</trd>,
       <trd>&c_r;&c_o;&c_g;&c_u;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>,
       <trd>&c_ch;&c_i;&c_l;&c_i;&c_m;</trd>,
       <trd>&c_v;&c_o;&c_d;&c_ja;&c_n;&c_o;&c_j; &c_k;&c_a;&c_sh;&c_t;&c_a;&c_n;</trd>
     </trdgrp>
     <trd lng="sv">sj&ouml;n&ouml;t</trd>
  </snc>
  <snc mrk="trap.0o.frukto">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="dif" cel="trap.0ofrukto"><tld lit="T"/>ofrukto</ref>:
      <ekz>
        mi ofte rememoradis pri la legomoj kaj fruktoj, kiujn mi man&gcirc;is kiel infano en
        naski&gcirc;loko: <tld/>oj, faboj...
        <fnt>
          <vrk><url ref="http://www.esperanto.org/literaturo/huguozhu/MF.chm">Matenaj Floroj Kolektitaj en Vesperokovrilo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
       <trd>chataigne d'eau</trd>,
       <trd>noix aquatique</trd>
    </trdgrp>
    <trdgrp lng="hu">
       <trd>csemegesulyom</trd>,
       <trd>v&iacute;zigesztenye</trd>
    </trdgrp>
    <trd lng="pt">castanha-de-&aacute;gua</trd>
    <trdgrp lng="nl">
       <trd>waternoot</trd>,
       <trd>waterkastanje</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_o;&c_d;&c_ja;&c_n;&c_o;&c_j; &c_o;&c_r;&c_je;&c_h;</trd>
    <trd lng="sv">sj&ouml;n&ouml;t<klr>sfrukt</klr></trd>
  </snc>
    <bld lok="&WCU;/thumb/2/2f/Illustration_Trapa_natans1.jpg/298px-Illustration_Trapa_natans1.jpg" prm="PD">botanika tabulo de <tld/>o (Trapa natans)
        <fnt>
          <aut>Prof. Dr. Otto Wilhelm Thom&eacute;: Flora von Deutschland, &Ouml;sterreich und der Schweiz 1885, Gera</aut>
          <lok><url ref="&WCW;/File:Illustration_Trapa_natans1.jpg">&WCW;/File:Illustration_Trapa_natans1.jpg</url></lok>
        </fnt>
    </bld>
  <trd lng="cs">asfaltov&yacute; holub</trd>
  <trd lng="sk">asfaltov&yacute; holub</trd>
  <trd lng="uk">&#x432;&#x43E;&#x434;&#x44F;&#x43D;&#x438;&#x439; &#x433;&#x43E;&#x440;&#x456;&#x445;</trd>
</drv>

<drv mrk="trap.0ofrukto">
   <kap><tld/>ofrukto, <var><kap><tld/>frukto</kap></var></kap>
   <snc>
     <dif>
       Dik&scirc;ela frukto de unu specio de <tld/>oj (Trapa
       natans) kun man&gcirc;ebla kerno.
     </dif>
     <ref tip="sin" cel="trap.0o.frukto"><tld/>o <sncref/></ref>
   </snc>
   <trdgrp lng="fr">
     <trd>chataigne d'eau</trd>,
     <trd>noix aquatique</trd>
   </trdgrp>
   <trd lng="de">Wassernuss<klr>frucht</klr></trd>
   <trdgrp lng="hu">
     <trd>csemegesulyom</trd>,
     <trd>v&iacute;zigesztenye</trd>
   </trdgrp>
   <trdgrp lng="nl">
       <trd>waternoot</trd>,
       <trd>waterkastanje</trd>
     </trdgrp>
   <trdgrp lng="pl">
       <trd>orzech kotewki</trd>,
       <trd>orzech wodny</trd>
     </trdgrp>
   <trd lng="pt">castanha-de-&aacute;gua</trd>
   <trd lng="ru">&c_v;&c_o;&c_d;&c_ja;&c_n;&c_o;&c_j; &c_o;&c_r;&c_je;&c_h;</trd>
   <trd lng="sv">sj&ouml;n&ouml;t<klr>sfrukt</klr></trd>
</drv>
</art>

