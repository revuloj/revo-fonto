<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <rad>trap</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="trap.0o">
   <kap><tld/>o</kap>
   <snc mrk="trap.0o.planto">
     <uzo tip="fak">BOT</uzo>
     <dif>
       Genro de longaj flosaj vegeta&jcirc;oj (<trd lng="la">Trapa</trd>)
       kun rombaj, dikaj folioj, blankaj floretoj, prosperaj en duonvarma,
       iom stagna akvo en Azio, E&ubreve;ropo, Afriko kaj pro homa enkonduko en
       Nordameriko kaj A&ubreve;stralio:
       <ekz>
         kiu povas flari bonan odoron el <tld/>a floro?
         <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 80a, volumo 2a, p.
         670a</lok></fnt>
       </ekz>
     </dif>
     <trdgrp lng="fr">
       <trd>macle</trd>,
       <trd>macre</trd>
     </trdgrp>
   </snc>
   <snc mrk="trap.0o.frukto">
     <uzo tip="fak">BOT</uzo>
     <ref tip="dif" cel="trap.0ofrukto"><tld/>ofrukto</ref>
     <trdgrp lng="fr">
       <trd>chataigne d'eau</trd>,
       <trd>noix aquatique</trd>
     </trdgrp>
   </snc>
</drv>

<drv mrk="trap.0ofrukto">
   <kap><tld/>ofrukto, <tld/>frukto</kap>
   <snc>
     <dif>
       Dik&scirc;ela frukto de unu specio de <tld/>oj (Trapa
       natans) kun man&gcirc;ebla kerno.
     </dif>
     <ref tip="sin" cel="trap.0o.frukto"><tld/>o <sncref/></ref>
   </snc>
   <trdgrp lng="fr">
     <trd>chataigne d'eau</trd>,
     <trd>noix aquatique</trd>
   </trdgrp>
</drv>
</art>

