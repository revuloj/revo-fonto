<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <rad>trap</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="trap.0o">
   <kap><tld/>o</kap>
   <snc mrk="trap.0o.planto">
     <uzo tip="fak">BOT</uzo>
     <dif>
       Genro de longaj flosaj vegeta&jcirc;oj (<trd lng="la">Trapa</trd>)
       kun rombaj, dikaj folioj, blankaj floretoj, prosperaj en duonvarma,
       iom stagna akvo en Azio, E&ubreve;ropo, Afriko kaj pro homa enkonduko en
       Nordameriko kaj A&ubreve;stralio:
       <ekz>
         kiu povas flari bonan odoron el <tld/>a floro?
         <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 80a, volumo 2a, p.
         670a</lok></fnt>
       </ekz>
     </dif>
     <trdgrp lng="fr">
       <trd>macle</trd>,
       <trd>macre</trd>
     </trdgrp>
     <trd lng="hu">sulyom</trd>
     <trd lng="de">Wassernuss<klr>Pflanze</klr></trd>
     <trdgrp lng="ru">
       <trd>&c_v;&c_o;&c_d;&c_ja;&c_n;&c_o;&c_j; &c_o;&c_r;&c_je;&c_h;</trd>,
       <trd>&c_r;&c_o;&c_g;&c_u;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>,
       <trd>&c_ch;&c_i;&c_l;&c_i;&c_m;</trd>,
       <trd>&c_v;&c_o;&c_d;&c_ja;&c_n;&c_o;&c_j; &c_k;&c_a;&c_sh;&c_t;&c_a;&c_n;</trd>
     </trdgrp>
   </snc>
   <snc mrk="trap.0o.frukto">
     <uzo tip="fak">BOT</uzo>
     <ref tip="dif" cel="trap.0ofrukto"><tld/>ofrukto</ref>
     <trdgrp lng="fr">
       <trd>chataigne d'eau</trd>,
       <trd>noix aquatique</trd>
     </trdgrp>
     <trdgrp lng="hu">
       <trd>csemegesulyom</trd>,
       <trd>v&iacute;zigesztenye</trd>
     </trdgrp>
       <trd lng="pt">castanha-de-&aacute;gua</trd>
     <trd lng="ru">&c_v;&c_o;&c_d;&c_ja;&c_n;&c_o;&c_j; &c_o;&c_r;&c_je;&c_h;</trd>
   </snc>
</drv>

<drv mrk="trap.0ofrukto">
   <kap><tld/>ofrukto, <tld/>frukto</kap>
   <snc>
     <dif>
       Dik&scirc;ela frukto de unu specio de <tld/>oj (Trapa
       natans) kun man&gcirc;ebla kerno.
     </dif>
     <ref tip="sin" cel="trap.0o.frukto"><tld/>o <sncref/></ref>
   </snc>
   <trdgrp lng="fr">
     <trd>chataigne d'eau</trd>,
     <trd>noix aquatique</trd>
   </trdgrp>
   <trd lng="de">Wassernuss <klr>Frucht</klr></trd>
   <trdgrp lng="hu">
     <trd>csemegesulyom</trd>,
     <trd>v&iacute;zigesztenye</trd>
   </trdgrp>
   <trd lng="ru">&c_v;&c_o;&c_d;&c_ja;&c_n;&c_o;&c_j; &c_o;&c_r;&c_je;&c_h;</trd>
</drv>
</art>

