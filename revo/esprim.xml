<vortaro>
<art mrk="$Id: esprim.xml,v 1.76 2021/06/18 20:41:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>esprim</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="esprim.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Montri sian penson a&ubreve; senton per paroloj, gestoj a&ubreve;
      aliaj
      a&ubreve;deblaj a&ubreve; videblaj signoj:
      <ekz>
        se mi volas <tld/>i direkton, mi uzas akuzativon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        liaj rigardoj <tld/>is teruron;
      </ekz>
      <ekz>
        <tld/>i koran dankon<fnt>Z</fnt>,
      </ekz>
      <ekz>
        sian favoron<fnt>Z</fnt>,
      </ekz>
      <ekz>
        opinion, deziron, &gcirc;ojon, kontentecon, doloron.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dir.el0i">eldiri</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_r;&c_a;&c_zh;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">expressar</trd>
  <trdgrp lng="cs">
    <trd>d&aacute;t najevo</trd>,
    <trd>projevit</trd>,
    <trd>projevit</trd>,
    <trd>projevovat</trd>,
    <trd>vyj&aacute;d&rcaron;it</trd>,
    <trd>vyjad&rcaron;ovat</trd>,
    <trd>vyj&aacute;d&rcaron;it</trd>,
    <trd>vyslovit</trd>,
    <trd>&rcaron;&iacute;ci</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ausdr&uuml;cken</trd>,
    <trd>&auml;u&szlig;ern</trd>
  </trdgrp>
  <trd lng="en">express</trd>
  <trd lng="es">expresar</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&f_y;&a_A;&a_n; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_A;&a_b;&a_r;&a_A;&a_z; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_A;&a_Z;&a_h;&a_A;&a_r; &f_k;&a_r;&a_d;&a_n;</trd>,
  </trdgrp>
  <trd lng="fr">exprimer<klr tip="ind"> (dire)</klr></trd>
  <trd lng="hu">kifejez</trd>
  <trd lng="pl">wyra&zdot;a&cacute;</trd>
  <trdgrp lng="pt">
    <trd>exprimir</trd>,
    <trd>expressar</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_y;&c_r;&c_a;&c_z;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>prejavi&tcaron;</trd>,
    <trd>vyjadri&tcaron;</trd>,
    <trd>vyslovi&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">uttrycka</trd>
</drv>

<drv mrk="esprim.0o">
  <kap><tld/>o</kap>
  <snc mrk="esprim.0o.ado">
    <dif>
      Montro de penso a&ubreve; sento per paroloj a&ubreve; gestoj:
      <ekz>
        mal&gcirc;oja <tld/>o ku&scirc;is sur &scirc;ia frunto.
      </ekz>
    </dif>
    <trdgrp lng="fa">
      <trd>&a_b;&f_y;&a_A;&a_n;</trd>,
      <trd>&a_A;&a_b;&a_r;&a_A;&a_z;</trd>,
      <trd>&a_A;&a_Z;&a_h;&a_A;&a_r;</trd>
    </trdgrp>
    <trd lng="pl">wyraz</trd>
  </snc>
  <snc mrk="esprim.0o.ajxo">
  <uzo tip="fak">LIN</uzo>
    <dif>
      Dirmaniero,
      <ref tip="vid" cel="formul.0o.vorta">formulo<sncref/></ref>,
      <ref tip="vid" cel="kompli1.0o.MOR">komplimento<sncref/></ref>:
      <ekz>
        tiu <tld/>o estas malklara, internacie nekomprenebla;
      </ekz><ekz>
      novaj <tld/>oj devas esti enkondukataj nur iom post iom,
      nerimarkeble<fnt><bib>LR</bib>, <lok>Respondo 25, La Revuo, 1907,
      Oktobro</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fa">
      <trd>&a_ayn;&a_b;&a_A;&a_r;&a_t;</trd>,
      <trd>&a_A;&a_S;&a_T;&a_l;&a_A;&a_H;</trd>
    </trdgrp>
    <trd lng="pl">wymowa</trd>
  </snc>
  <snc mrk="esprim.0o.KOMP">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Sinsekvo da <ref cel="simbol.0o.SCI">simboloj</ref>, indikanta 
      matematikan a&ubreve; programan objekton:
      <ekz>
        la <tld/>o <frm am="a^x-2"><k>a</k><sup><k>x</k></sup>-2</frm> 
        konsistas el kvar 
        simboloj kaj prezentas rezulton de du sinsekvaj operacioj;
      </ekz>
      <ekz>
        kalkulu la <tld/>on <frm am="a^2+5a-2"><k>a</k><sup>2</sup>+5<k>a</k>-2</frm>,       
        kiam <frm am="a=2"><k>a</k> = 2</frm>;
      </ekz>
      <ekz>
        egala&jcirc;o konsistas el du <tld/>oj kunigitaj per egalsigno.
      </ekz>
    </dif>
    <refgrp tip="sub">
      Komparu kun:
      <ref cel="formul.0o.MAT">formulo</ref>.
    </refgrp>
    <trd lng="fa">&a_ayn;&a_b;&a_A;&a_r;&a_t;</trd>
    <trdgrp lng="pl">
      <trd>wyra&zdot;enie</trd>,
      <trd>wyra&zdot;enie<klr tip="ind"> (algebraiczne)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="af">frase</trd>
  <trd lng="am">&#x1210;&#x1228;&#x130D;</trd>
  <trd lng="ar">&#x627;&#x644;&#x639;&#x628;&#x627;&#x631;&#x629;</trd>
  <trd lng="az">s&#xF6;z</trd>
  <trd lng="be">&c_v;&c_y;&c_r;&c_a;&c_z;</trd>
  <trd lng="bn">&#x9AB;&#x9CD;&#x9B0;&#x9C7;&#x99C;</trd>
  <trd lng="bs">frazu</trd>
  <trd lng="ca">expressi&oacute;</trd>
  <trd lng="co">scritta</trd>
  <trdgrp lng="cs">
    <trd>projev</trd>,
    <trd>v&yacute;raz</trd>,
    <trd>vyj&aacute;d&rcaron;en&iacute;</trd>
  </trdgrp>
  <trd lng="da">s&#xE6;tning</trd>
  <trd lng="de">Ausdruck</trd>
  <trd lng="en">expression</trd>
  <trd lng="es">expresi&oacute;n</trd>
  <trd lng="et">fraasi</trd>
  <trd lng="eu">esaldi</trd>
  <trd lng="fr">expression</trd>
  <trd lng="gl">frase</trd>
  <trd lng="gu">&#xAAE;&#xAB9;&#xABE;&#xAB5;&#xAB0;&#xACB;</trd>
  <trd lng="ha">magana</trd>
  <trdgrp lng="he">
    <trd>&#x5D1;&#x5D9;&#x5D8;&#x5D5;&#x5D9;</trd>,
    <trd>&#x5D4;&#x5D1;&#x5E2;&#x5D4;</trd>
  </trdgrp>
  <trd lng="hi">&#x935;&#x93E;&#x915;&#x94D;&#x92F;&#x93E;&#x902;&#x936;</trd>
  <trd lng="hr">fraza</trd>
  <trd lng="hu">kifejez&eacute;s</trd>
  <trd lng="hy">&#x561;&#x580;&#x57F;&#x561;&#x570;&#x561;&#x575;&#x57F;&#x578;&#x582;&#x569;&#x575;&#x578;&#x582;&#x576;</trd>
  <trd lng="is">setning</trd>
  <trd lng="ja">&#x30D5;&#x30EC;&#x30FC;&#x30BA;</trd>
  <trd lng="jw">tembung</trd>
  <trd lng="ka">&#x10E4;&#x10E0;&#x10D0;&#x10D6;&#x10D0;</trd>
  <trd lng="km">&#x1783;&#x17D2;&#x179B;&#x17B6;</trd>
  <trd lng="ko">&#xBB38;&#xAD6C;</trd>
  <trd lng="lo">&#xE9B;&#xEB0;&#xEC2;&#xEAB;&#xE8D;&#xE81;</trd>
  <trd lng="ml">&#xD36;&#xD48;&#xD32;&#xD3F;</trd>
  <trd lng="mr">&#x935;&#x93E;&#x915;&#x94D;&#x92F;&#x93E;&#x902;&#x936;</trd>
  <trd lng="my">&#x1011;&#x102C;&#x1038;&#x101E;&#x1031;&#x102C;&#x1005;&#x102C;&#x1015;&#x102D;&#x102F;&#x1012;&#x103A;&#x1010;&#x102D;&#x102F;&#x1019;&#x103B;&#x102C;&#x1038;</trd>
  <trd lng="ne">&#x935;&#x93E;&#x915;&#x94D;&#x92F;&#x93E;&#x902;&#x936;</trd>
  <trd lng="ny">mawu</trd>
  <trd lng="pa">&#xA35;&#xA3E;&#xA15;&#xA70;&#xA38;&#xA3C;</trd>
  <trd lng="ps">&#x639;&#x628;&#x627;&#x631;&#x62A;</trd>
  <trd lng="ru">&c_v;&c_y;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sd">&#x62C;&#x645;&#x644;&#x64A;</trd>
  <trd lng="si">&#xDC0;&#xDD0;&#xD9A;&#xDD2;&#xDBA;</trd>
  <trdgrp lng="sk">
    <trd>kombin&aacute;cia znakov</trd>,
    <trd>v&yacute;raz</trd>
  </trdgrp>
  <trd lng="sq">fraza</trd>
  <trd lng="st">poleloana e reng</trd>
  <trd lng="sv">uttryck</trd>
  <trd lng="ta">&#xB9A;&#xBCA;&#xBB1;&#xBCD;&#xBB1;&#xBCA;&#xB9F;&#xBB0;&#xBCD;</trd>
  <trd lng="te">&#xC2A;&#xC26;&#xC2C;&#xC02;&#xC27;&#xC02;</trd>
  <trd lng="th">&#xE27;&#xE25;&#xE35;</trd>
  <trd lng="tl">parirala</trd>
  <trd lng="ur">&#x62C;&#x645;&#x644;&#x6C1;</trd>
  <trd lng="vi">c&#x1EE5;m t&#x1EEB;</trd>
  <trd lng="yi">&#x5E4;&#x5E8;&#x5D0;&#x5B7;&#x5D6;&#x5E2;</trd>
  <trd lng="zu">isisho</trd>
</drv>

<drv mrk="esprim.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">MAT</uzo>
  <uzo tip="stl">ARK</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 15</lok></fnt>
  <snc mrk="esprim.0ajxo.MAT">
    <ref tip="dif" cel="esprim.0o.KOMP"><tld/>o.</ref>
    <trd lng="fa">&a_ayn;&a_b;&a_A;&a_r;&a_t;</trd>
    <trd lng="pl">wyra&zdot;enie<klr tip="ind">
    (algebraiczne)</klr></trd>
  </snc>
</drv>

<drv mrk="esprim.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="esprim.0ilo.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      Lingva rimedo por esprimi iun sencon a&ubreve; nuancon:
      <ekz>
        la penso starigas inter ideoj certan nombron da rilatoj,
        por kiuj la naciaj lingvoj ne &ccirc;iam disponas <tld/>ilojn
        <fnt>
          <aut>G. Waringhien</aut>,
          <vrk>Lingvo kaj vivo</vrk>,
          <lok>1989</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">KOMP</uzo>
        rekursiaj funkcioj estas eleganta <tld/>ilo por prezenti
        multajn klasikajn algoritmojn.
      </ekz>
    </dif>
    <trd lng="de">Ausdrucksm&ouml;glichkeit</trd>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_ja;&c_w;&c_l;&c_je;&c_n;&c_ch;&c_y;
  &c_s;&c_r;&c_o;&c_d;&c_a;&c_k;</trd>
  <trd lng="ca">mitj&agrave; d'<ind>expressi&oacute;</ind></trd>
  <trd lng="cs">v&yacute;razov&yacute; prost&rcaron;edek</trd>
  <trdgrp lng="en">
    <trd>expressive means</trd>,
    <trd><klr>(language)</klr> feature</trd>
  </trdgrp>
  <trd lng="es">medios de <ind>expresi&oacute;n</ind></trd>
  <trd lng="fa">&a_A;&a_b;&a_z;&a_A;&a_r; <ind>&a_b;&f_y;&a_A;&a_n;</ind></trd>
  <trd lng="fr">moyen d'<ind>expression</ind></trd>
  <trd lng="hu">kifejez&odblac;eszk&ouml;z</trd>
  <trdgrp lng="pl">
    <trd>wyra&zdot;enie</trd>,
    <trd>&sacute;rodek <ind>wyrazu</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_o;&c_b;&c_r;&c_a;&c_z;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je;
    &c_s;&c_r;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_s;&c_r;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o;
    &c_v;&c_y;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">v&yacute;razov&yacute; prostriedok</trd>
</drv>

<drv mrk="esprim.0isma">
  <kap><tld/>isma</kap>
  <snc mrk="esprim.0isma.BELA">
    <dif>
      Konforma al la stilo de <tld/>ismo, el la <tld/>ismo:
      <ekz>
        <ctl>La krio</ctl> estas unu el la plej reprezentaj
        arta&jcirc;oj de 
        la <tld/>isma movado
        <fnt>
          <aut>&Gcirc;angalo</aut>,
          <vrk><url ref="http://gxangalo.com/modules/news/article.php?storyid=1654">
          Oni ofertas konsiderindan monrekompencon...</url></vrk>,
          <lok>2004-08-25</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">expressionistisch</trd>
  </snc>
  <trd lng="ca">expressionista</trd>
  <trd lng="es">expresionista</trd>
  <trd lng="fa">&a_A;&f_k;&a_s;&f_p;&a_r;&a_s;&f_y;&a_w;&a_n;&f_y;&a_s;&a_t;</trd>
  <trd lng="fr">expressioniste<klr> (adj.)</klr></trd>
  <trd lng="it">espressionistico</trd>
</drv>

<drv mrk="esprim.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="esprim.0ismo.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Arta stilo, kiu forte prezentas sentojn kaj emociojn,
      e&ccirc; per troigo a&ubreve; karikaturo, historie precipe germandevena
      movado el la unua duono de la 20a jarcento; anka&ubreve; nomata
      <ref tip="sin" cel="ekspresionism.0o.BELA">ekspresionismo</ref>:
      <ekz>
        <tld/>ismo velkos kun na&ubreve;zo / post la&ubreve;to ekestos
        kontemplo
        <fnt>
          <aut>J. S. Skorupski</aut>,
          <vrk>Soneto je kelkaj kontemploj</vrk>,
          <lok><url ref="http://www.skorupski.com/sonets/index.php?lang=eo">Sonetado</url>,
          1995-07-20</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Mikl&oacute;s Radn&oacute;ti</nom> <klr>[&#x2026;]</klr> debutis kiel
        avantgarda poeto, 
        liaj fruaj poemoj montras la influojn de <tld/>ismo kaj 
        superrealismo
        <fnt>
          <aut>N. Ruggiero</aut>,
          <vrk><url ref="http://www.nicolaruggiero.it/html/eo/">taglibro
          de nenifaranto</url></vrk>, 
          <lok>2005-11-06</lok>
        </fnt>.
      </ekz> 
    </dif>
    <trd lng="de">Expressionismus</trd>
  </snc>
  <trd lng="ca">expressionisme</trd>
  <trd lng="es">expresionismo</trd>
  <trd lng="fa">&a_A;&f_k;&a_s;&f_p;&a_r;&a_s;&f_y;&a_w;&a_n;&f_y;&a_s;&a_m;</trd>
  <trd lng="fr">expressionisme</trd>
  <trd lng="it">espressionismo</trd>
</drv>

<drv mrk="esprim.dank0o">
  <kap>dank<tld/>o</kap>
  <snc mrk="esprim.dank0o.KOMUNE">
    <dif>
      Pli-malpli formalo <ref tip="super" cel="esprim.0o.ajxo">esprimo</ref>
      de danko:
      <ekz>
        &ccirc;iu kreita&jcirc;o de Dio estas bona, kaj nenio devas esti for&jcirc;etita,
        se oni &gcirc;in ricevas kun <tld/>kesprimo
        <fnt>
          <bib>NT</bib>
          <lok>I. Timoteo 4:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        per pre&gcirc;o kaj peto kun <tld/>esprimo, viaj deziroj scii&gcirc;u al Dio
        <fnt>
          <bib>NT</bib>
          <lok>Filipianoj 4:6</lok>
        </fnt>;
      </ekz>      
      <ekz>
        li lasis lin kun sinceraj dank<tld/>oj
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>, 
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Danksagung</trd>
  </snc>
  <trd lng="fr">formule de remerciement</trd>
  <trd lng="he">&#x5EA;&#x5D5;&#x5D3;&#x5D4;</trd>
</drv>

<drv mrk="esprim.sub0o">
  <kap>sub<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <uzo tip="fak">KOMP</uzo>
  <snc>
    <dif>
      <tld lit="E"/>o rigardata kiel parto de alia <tld/>o:
      <ekz>
        &scirc;pari komunajn sub<tld/>ojn en programo;
      </ekz>
      <ekz>
        redukti egalajn sub<tld/>ojn en algebra frakcio.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_v;&c_y;&c_r;&c_a;&c_z;</trd>
  <trd lng="ca">subexpressi&oacute;<klr> (inform.)</klr></trd>
  <trd lng="de">Term</trd>
  <trd lng="en">subexpression</trd>
  <trd lng="es">subexpresi&oacute;n</trd>
  <trd lng="fa">&a_z;&f_y;&a_r;&a_ayn;&a_b;&a_A;&a_r;&a_t;</trd>
  <trd lng="fr">sous-expression</trd>
  <trd lng="hu">r&eacute;szkifejez&eacute;s</trd>
  <trd lng="pl">podwyra&zdot;enie</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_v;&c_y;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="esprim.vizagx0o">
  <kap>viza&gcirc;<tld/>o</kap>
  <snc mrk="esprim.vizagx0o.MIEN">
    <dif>
      <ref tip="super" cel="esprim.0o.ado">Esprimo</ref>
      de emocio, sento, opinio videbla sur
      la momenta &scirc;ajno de la viza&gcirc;o;
      <ref tip="vid" cel="fizion.0o.MIEN">fizionomio<sncref/></ref>:
      <ekz>
        li <klr>...</klr> havis la viza&gcirc;<tld/>on de
        kaprica infano, kiu tuj ekploros
        <fnt>
          <aut>L. Karpunina</aut>,
          <vrk>Neokazinta amo</vrk>,
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>figure<klr> (expression du visage)</klr></trd>,
    <trd>physionomie</trd>
  </trdgrp>
</drv>
</art>
