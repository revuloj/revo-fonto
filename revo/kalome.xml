<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>kalomel</rad>/o
</kap>
<drv mrk="kalome.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Hg<sub>2</sub>Cl<sub>2</sub>. Hidrarga protoklorido, en pasinteco
      uzata kiel laksigilo a&ubreve; kontra&ubreve;vermilo.  
    </dif>
  </snc>
  <adm>
    Klorido kaj protoklorido aperantaj en dif ne estas difinitaj.
    <aut>MB</aut>
  </adm>
  <trd lng="de">Quecksilberchlorid</trd>
  <trd lng="en">calomel</trd>
  <trd lng="hu">kalomel</trd>
  <trd lng="fr">calomel</trd>
  <trd lng="nl">kalomel</trd>
  <trd lng="pt">calomelano</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_l;&c_o;&c_m;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_h;&c_l;&c_o;&c_r;&c_i;&c_s;&c_t;&c_a;&c_ja; &c_r;&c_t;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
