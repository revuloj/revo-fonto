<drv mrk="klemat1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <fnt><bib>PV</bib></fnt>
  <snc>
    <dif>
      Genro el familio <ref tip="super"
      cel="ranunk1.0oj">ranunkolacoj</ref>
