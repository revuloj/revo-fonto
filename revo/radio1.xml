  <snc mrk="radio1.0o.radiofonio">
    <dif>
      <ref tip="dif" cel="radiof.0o">Radiofonio</ref>:
      <ekz>
        helpokrio (per <tld/>o)
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        pluraj <ind><tld/>o-stacio</ind>j enkondukis kursojn de la Internacia Lingvo
        <fnt>
          <bib>EeP</bib>
          <lok>Unua Parto</lok>
        </fnt>;
        <trd lng="de">Rundfunksender</trd>
      </ekz>
      <ekz>
        varsovia solena&jcirc;o, kiun oni povis spekti en la televido kaj a&ubreve;skulti en la <tld/>o
        <fnt>
          <aut>Sylvia Brylska</aut>
          <vrk><url ref="&Monato;/1993/A930801.php">&Ccirc;u reiri al naciaj
          radikoj?</url></vrk>
          <bib>Monato</bib>
          <lok>1993/08, p. 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        Pola <tld lit="R"/>o persatelite elsendas esperantlingvajn programojn
        <fnt>
          <aut>Pejno Simono</aut>
          <vrk><url ref="&Monato;/1995/004358.html">Poloj per pelvo</url></vrk>
          <bib>Monato</bib>
          <lok>1995/04, p. 17</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oprogramoj
        <fnt>
          <aut>Pejno Simono</aut>
          <vrk><url ref="&Monato;/1995/004358.html">Poloj per pelvo</url></vrk>
          <bib>Monato</bib>
          <lok>1995/04, p. 17</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o-amatora terminaro
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li laboras &ccirc;e la <tld/>o
        <fnt>
          <bib>ChV</bib>
          <lok>10</lok>
        </fnt>;
      </ekz>
      <ekz>
        li havas unumonatan kontrakton kun la <tld/>o-orkestro
        <fnt>
          <bib>ChV</bib>
          <lok>15</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_d;&c_y;&c_jo;&c_v;&c_ja;&c_sh;&c_ch;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
