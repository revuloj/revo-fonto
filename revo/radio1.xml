      <trd>radiofonie</trd>
    </trdgrp>
      <trd lng="pt">radiofonia</trd>
    <trd lng="ru">&c_r;&c_a;&c_d;&c_i;&c_o;&c_v;&c_je;&c_shch;&c_a;&c_n;&c_i;&c_je;</trd>
