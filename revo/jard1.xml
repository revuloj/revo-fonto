<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>jard</rad>/o
</kap>
<drv mrk="jard1.0o">
  <kap><tld/>o</kap>
  <snc mrk="jard1.0o.MEZ">
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de
      longo uzata en anglolingvaj landoj:
    </dif>
    <subsnc mrk="jard1.0o.MEZ_brita">
      <dif>
        La brita <tld/>o egalas ekde 1922 al 0,9143984 metro.
      </dif>
    </subsnc>
    <subsnc mrk="jard1.0o.MEZ_usona">
      <dif>
        La usona <tld/>o egalas ekde 1959 al 0,9144 metro.
      </dif>
    </subsnc>
  </snc>
  <rim mrk="jard1.0o.RIM_HOM">
    <ref tip="hom" cel="jard.0o">Jardo</ref> anka&ubreve; estas la nomo de peco en vel&scirc;ipo.
  </rim>
  <trd lng="be">&c_ja;&c_r;&c_d;</trd>
  <trd lng="de">Yard</trd>
  <trd lng="en">yard</trd>
  <trd lng="es">yarda</trd>
  <trdgrp lng="fr">
    <trd>yard</trd>
  </trdgrp>
  <trd lng="hu">yard</trd>
  <trd lng="nl">voet<klr tip="ind"> (lengtem.)</klr></trd>
  <trd lng="pt">jarda</trd>
  <trd lng="ru">&c_ja;&c_r;&c_d;</trd>
  <trd lng="sv">yard</trd>
</drv>
</art>

