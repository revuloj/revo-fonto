<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: jard1.xml,v 1.14 2023/10/18 05:25:55 revo Exp $">
<kap>
  <rad>jard</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="jard1.0o">
  <kap><tld/>o</kap>
  <snc mrk="jard1.0o.MEZ">
    <dif>
      <ref tip="lst" cel="unu.mezur0o" lst="voko:longoj">Mezurunuo</ref> de
      longo uzata en anglolingvaj landoj:
      <ekz>
        anta&ubreve; multaj jaroj ili vendis la lastan <tld/>on da drapo
        <fnt>
        <aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>,
        <vrk>Cent Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>
    </dif>
    <subsnc mrk="jard1.0o.MEZ_brita">
      <dif>
        La brita <tld/>o egalas ekde 1922 al 0,9143984 metro.
      </dif>
    </subsnc>
    <subsnc mrk="jard1.0o.MEZ_usona">
      <dif>
        La usona <tld/>o egalas ekde 1959 al 0,9144 metro.
      </dif>
    </subsnc>
  </snc>
  <rim mrk="jard1.0o.RIM_HOM">
    <ref tip="hom" cel="jard.0o">Jardo</ref> anka&ubreve; estas la nomo
    de peco en vel&scirc;ipo.
  </rim>
  <trd lng="be">&c_ja;&c_r;&c_d;</trd>
  <trd lng="bg">&c_ja;&c_r;&c_d;</trd>
  <trd lng="ca">iarda</trd>
  <trd lng="de">Yard</trd>
  <trd lng="en">yard</trd>
  <trd lng="es">yarda</trd>
  <trd lng="fr">yard</trd>
  <trd lng="hu">yard</trd>
  <trd lng="id">yard</trd>
  <trdgrp lng="ja">
    <trd>&#x30E4;&#x30FC;&#x30C9;</trd>,
    <trd>&#x5E06;&#x6841;  <pr>&#x307B;&#x3052;&#x305F;</pr></trd>
  </trdgrp>
  <trd lng="nl">yard</trd>
  <trd lng="pl">jard</trd>
  <trd lng="pt">jarda</trd>
  <trd lng="ru">&c_ja;&c_r;&c_d;</trd>
  <trd lng="sv">yard</trd>
  <trd lng="it">iarda</trd>
  <trd lng="sk">rahno</trd>
  <trd lng="cs">r&aacute;hno</trd>
  <trdgrp lng="uk">
    <trd>&#x440;&#x435;&#x44F;</trd>,
    <trd>&#x44F;&#x440;&#x434; (&#x43C;&#x456;&#x440;&#x430; &#x434;&#x43E;&#x432;&#x436;&#x438;&#x43D;&#x438;)</trd>
  </trdgrp>
</drv>
</art>

