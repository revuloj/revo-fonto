<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>jard</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="jard1.0o">
  <kap><tld/>o</kap>
  <snc mrk="jard1.0o.MEZ">
    <dif>
      <ref tip="lst" cel="unu.mezur0o"
      lst="voko:longoj">Mezurunuo</ref> de
      longo uzata en anglolingvaj landoj:
      <ekz>
        anta&ubreve; multaj jaroj ili vendis la lastan <tld/>on da drapo
        <fnt>
        <aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>,
        <vrk>Cent Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>
    </dif>
    <subsnc mrk="jard1.0o.MEZ_brita">
      <dif>
        La brita <tld/>o egalas ekde 1922 al 0,9143984 metro.
      </dif>
    </subsnc>
    <subsnc mrk="jard1.0o.MEZ_usona">
      <dif>
        La usona <tld/>o egalas ekde 1959 al 0,9144 metro.
      </dif>
    </subsnc>
  </snc>
  <rim mrk="jard1.0o.RIM_HOM">
    <ref tip="hom" cel="jard.0o">Jardo</ref> anka&ubreve; estas la nomo
    de peco en vel&scirc;ipo.
  </rim>
  <trd lng="be">&c_ja;&c_r;&c_d;</trd>
  <trd lng="ca">iarda</trd>
  <trd lng="de">Yard</trd>
  <trd lng="en">yard</trd>
  <trd lng="es">yarda</trd>
  <trd lng="fr">yard</trd>
  <trd lng="hu">yard</trd>
  <trd lng="id">yard</trd>
  <trd lng="nl">voet<klr tip="ind"> (lengtem.)</klr></trd>
  <trd lng="pl">jard</trd>
  <trd lng="pt">jarda</trd>
  <trd lng="ru">&c_ja;&c_r;&c_d;</trd>
  <trd lng="sv">yard</trd>
  <trd lng="it">iarda</trd>
</drv>
</art>

