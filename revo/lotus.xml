<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>lotus</rad>/o
</kap>
<drv mrk="lotus.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="lotus.0o.fabacano">
    <dif>
      Genro el familio <ref tip="super" cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Lotus</trd>), kreskanta en arbaroj, kampoj.
    </dif>
    <bld lok="../bld/lotus.jpg">
      <ind>ordinara <tld/>o</ind>
      <trd lng="la">Lotus corniculatus</trd>
    </bld>
    <trd lng="de">Hornklee</trd>
    <trd lng="hu">kerep</trd>
    <trd lng="nl">rolklaver</trd>
    <trd lng="ru">&c_l;&c_ja;&c_d;&c_v;&c_je;&c_n;&c_je;&c_c;</trd>
  </snc>
  <snc mrk="lotus.0o.akvoplantoj">
    <dif>
      Difinitaj specioj de akvoplantoj
      el <ref tip="malprt" cel="nimfea.0oj">nimfeacoj</ref>,
      nome la hinda <tld/>o (<trd lng="la">Nelumbo nucifera</trd>),
      la egipta <tld/>o (<trd lng="la">Nymphaea lotus</trd>)
      kaj la blua <tld/>o (<trd lng="la">Nymphaea caerulea</trd>).
    </dif>
    <ref tip="vid" cel="nimfe.0o">nimfeo</ref>
    <trd lng="de">Lotos</trd>
    <trdgrp lng="hu">
      <trd>t&uuml;nd&eacute;rr&oacute;zsa</trd>,
      <trd>l&oacute;tusz</trd>,
      <trd>padma</trd></trdgrp>
      <trd lng="nl">lotus</trd>
    <trd lng="ru">&c_l;&c_o;&c_t;&c_o;&c_s;</trd>
  </snc>
  <snc mrk="lotus.0o.arbo">
    <dif>
      Arbo, kies fruktoj, la&ubreve; kredo de la antikvuloj, forgesigis la
      patrolandon al tiuj, kiuj ilin man&gcirc;is. 
      =<ref tip="sin" cel="lotos.0o">lotoso</ref>
    </dif>
    <trd lng="de">Lotos</trd>
    <trd lng="hu">l&oacute;tuszfa</trd>
    <trd lng="nl">lotusboom</trd>
    <trd lng="ru">&c_l;&c_o;&c_t;&c_o;&c_s;</trd>
  </snc>
</drv>
</art>
