<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>apel</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="apel.0i">
  <kap><tld/>i</kap>
  <snc mrk="apel.0i.posteni">
    <uzo tip="fak">MIL</uzo>
    <dif>
      <klr>(pp soldatoj)</klr> Stari vice senmove por ricevi instrukciojn
      de superulo:
      <ekz>
        policisto <klr>[&#8230;]</klr>, kiu <tld/>is sur la trotuaro
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        kontrolado dum <ind><tld/>o</ind>, 2012
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=527351">58-a armeo (Rusio)</url></lok>
        </fnt>.
        <trd lng="de"><klr>(Fahnen-)</klr>Appell</trd>
      </ekz>
    </dif>
    <trd lng="de">zum <ind>Appell</ind> antreten</trd>
  </snc>

  <snc mrk="apel.0i.JUR">
    <uzo tip="stl">EVI</uzo>
    <uzo tip="fak">JUR</uzo>
    <ref tip="dif" cel="apelac.0i">apelacii</ref>
  </snc>
  <trd lng="cs">apelovat</trd>
  <trd lng="sk">apelova&tcaron;</trd>
</drv>
</art>
