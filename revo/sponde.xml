<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>sponde</rad>/o
</kap>
<drv mrk="sponde.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="pied.vers0o">Verspiedo</ref> konsistanta
      el du longaj a&ubreve; akcentaj silaboj; simb. /&dash;&dash;/:
      <ekz>
        &ccirc;ar du akcentaj silaboj ne povas stari sinsekve, en E-o
        la <tld/>o ne estas ebla <klr>(krom en okazo de
        <ref cel="karamb.0o.licenco">karambolo</ref>)</klr>
        <fnt><aut>&K;</aut><vrk>Lingvo, Stilo, Formo; Budapest,
        1931</vrk><lok>p. 131</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Spondeus</trd>
  <trd lng="fr">spond&eacute;e</trd>
  <trd lng="nl">spondee</trd>
  <trd lng="ru">&c_s;&c_p;&c_o;&c_n;&c_d;&c_je;&c_j;</trd>
</drv>
</art>
