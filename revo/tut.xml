    <trd>integridad</trd>,
    <trd>todo <klr>(subst.)</klr></trd>
  </trdgrp>
  <trd lng="fr">tout<klr tip="amb"> (subst., un tout, une
  unit&eacute;)</klr></trd>
  <trdgrp lng="pl">
    <trd>ca&lstroke;o&sacute;&cacute;</trd>,
    <trd>ca&lstroke;okszta&lstroke;t</trd>,
    <trd>og&oacute;&lstroke;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_s;&c_jo;</trd>,
    <trd>&c_c;&c_je;&c_l;&c_o;&c_je;</trd>,
    <trd>&c_v;&c_s;&c_jo; &c_v; &c_c;&c_je;&c_l;&c_o;&c_m;</trd>
  </trdgrp>
  <trd lng="sk">celistvos&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&c_s;&c_je;</trd>,
    <trd>&c_c;&c_ib;&c_l;&c_je;</trd>,
    <trd>&c_s;&c_u;&c_k;&c_u;&c_p;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="tut.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Io, al kio mankas neniu parto kaj kiu konsistigas apartan unuecan
      kuna&jcirc;on:
      <ekz>
        la <tld/>a&jcirc;on de la &scirc;ipoj de iu lando oni nomas
        &scirc;iparo<fnt>B</fnt>;
      </ekz>
      <ekz>
        li faris kvindek kuprajn hoketojn, por kunigi la tendon, por ke
        &gcirc;i
        estu unu <tld/>a&jcirc;o
        <fnt><bib>MT</bib>, <lok>&Eli; 36:18</lok></fnt>;
      </ekz>
      <ekz>
        oni disrompas vazon de potisto, ke &gcirc;i ne povas esti
        <tld/>a&jcirc;igita
        <fnt><bib>MT</bib>, <lok>&Jer; 19:11</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_e;&c_l;&c_a;&c_je;
    <klr>(&c_n;&c_a;&c_z;&c_o;&c_w;&c_n;&c_ib;&c_k;)</klr></trd>,
