    <trd>todo <klr>(subst.)</klr></trd>
  </trdgrp>
  <trd lng="fr">tout<klr tip="amb"> (subst., un tout, une
  unit&eacute;)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#20840;&#x90E8;  <pr>&#x305C;&#12435;&#12406;</pr></trd>,
    <trd>&#20840;&#x4F53;  <pr>&#x305C;&#12435;&#x305F;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ca&lstroke;o&sacute;&cacute;</trd>,
    <trd>ca&lstroke;okszta&lstroke;t</trd>,
    <trd>og&oacute;&lstroke;</trd>
  </trdgrp>
  <trdgrp lng="ru">
