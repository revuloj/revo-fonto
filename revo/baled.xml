<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>baled</rad>/o
    </kap>

    <drv mrk="baled.0o">
      <kap><tld/>o <fnt><bib>PV</bib></fnt></kap>
      <snc>
	<uzo tip="stl">RAR</uzo>
	<dif>
	  <ref tip="dif" cel="balet.0o">Baleto</ref>:
	  <ekz>
	    la <tld/>o <ctl>Du jarsezonoj</ctl> subtenas tiun ideon
	    kaj proponas vizion pri &gcirc;i
	    <fnt><lok><url
	    ref="http://www.uea.org/esperanto_p/servoj/kongresoj/2002/kl-arto.html"
		  >Kongresa Libro 2002</url></lok></fnt>;
	  </ekz><ekz>
	    oni vidis &scirc;in en &Hcirc;arkovo kun <tld/>estro
	    <fnt><bib>MkM</bib>, <lok>p. 356</lok></fnt>.
	  </ekz>
	</dif>
      </snc>
      <trd lng="be">&c_b;&c_a;&c_l;&c_je;&c_t;</trd>
      <trd lng="de">Ballet</trd>
      <trd lng="ru">&c_b;&c_a;&c_l;&c_je;&c_t;</trd>
    </drv>
  </art>
