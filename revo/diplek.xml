<vortaro>
<art mrk="$Id$">
<kap>
<rad>dipleks</rad>/a
</kap>
<drv mrk="diplek.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <uzo tip="klr">(pri sendaparato)</uzo>
    <dif>
      Povanta efektivigi samtempe du malsamajn sendojn sur du malsamaj
      ondlongoj;
      <ref tip="dif" cel="duplek.0a">dupleksa</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_u;&c_p;&c_l;&c_je;&c_k;&c_s;&c_n;&c_y;</trd>
  <trd lng="hu">diplex</trd>
  <trdgrp lng="pl">
    <trd>dwukierunkowy</trd>,
    <trd>duplex</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>d&uacute;plex</trd>,
    <trd>duplex</trd>
  </trdgrp>
  <trd
  lng="ru">&c_d;&c_u;&c_p;&c_l;&c_je;&c_k;&c_s;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
