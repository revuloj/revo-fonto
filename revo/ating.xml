  <trdgrp lng="cs">
    <trd>doc&iacute;lit</trd>,
    <trd>dos&aacute;hnout</trd>,
    <trd>zastihnout</trd>
  </trdgrp>
  <trd lng="da">n&#xE5;</trd>
  <trd lng="de">erreichen</trd>
  <trdgrp lng="en">
    <trd>attain</trd>,
    <trd>achieve<klr> (reach)</klr></trd>
    <trd>fulfill<klr> (goal)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>conseguir</trd>,
    <trd>lograr</trd>,
    <trd>alcanzar</trd>
  </trdgrp>
  <trd lng="et">j&#xF5;uda</trd>
  <trd lng="eu">iristeko</trd>
  <trd lng="fr">atteindre</trd>
  <trd lng="fy">berikke</trd>
  <trd lng="gl">alcanzar</trd>
  <trd lng="gu">&#xAB8;&#xAC1;&#xAA7;&#xAC0; &#xAAA;&#xAB9;&#xACB;&#xA82;&#xA9A;&#xAB5;&#xABE;</trd>
  <trd lng="ha">kai</trd>
  <trdgrp lng="he">
    <trd>&#x5DC;&#x5D4;&#x5D2;&#x5D9;&#x5E2;</trd>,
    <trd>&#x5DC;&#x5D4;&#x5E9;&#x5D9;&#x5D2;</trd>
  </trdgrp>
  <trd lng="hi">&#x924;&#x915; &#x92A;&#x939;&#x941;&#x901;&#x91A;&#x928;&#x947;</trd>
  <trd lng="hr">dosti&#x107;i</trd>
  <trd lng="hu">el&eacute;r</trd>
  <trd lng="hy">&#x570;&#x561;&#x57D;&#x576;&#x565;&#x56C;</trd>
