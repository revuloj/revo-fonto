    <trd>inaccesible</trd>,
    <trd>inalcanzable</trd>
  </trdgrp>
  <trd lng="fr">inaccessible</trd>
  <trdgrp lng="sk">
    <trd>nedostupn&yacute;</trd>,
    <trd>nepr&iacute;stupn&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="ating.kur0i">
