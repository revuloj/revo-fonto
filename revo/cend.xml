<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>cend</rad>/o
</kap>
<drv mrk="cend.0o">
<kap><tld/>o</kap>
<uzo tip="fak">EKON</uzo>
<snc>
<dif>
<ref tip="super" cel="unu.monasub0o">Mona subunuo</ref>, egala al centono de
<ref tip="malprt" cel="dolar.0o">dolaro</ref>,
<ref tip="malprt" cel="euxr.0o">e&ubreve;ro</ref>,
<ref tip="malprt" cel="gulden.0o">guldeno</ref>;
tiomvalora <ref tip="super" cel="mon.0ero">monero</ref>.
</dif>
<trd lng="el">&lambda;&epsilon;&pi;&tau;&omikron_ton;</trd>
<trd lng="es">centavo</trd></snc>
<trdgrp lng="fr">
  <trd>cent<klr tip="ind"> (centi&egrave;me de dollar, etc.)</klr></trd>,
  <trd>centime<klr tip="amb"> (d'euro)</klr></trd></trdgrp>
<trd lng="nl">cent</trd>
<trd lng="pt">centavo</trd>
<trd lng="ru">&c_c;&c_je;&c_n;&c_t;</trd>
</drv>
</art>
