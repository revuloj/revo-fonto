<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>cend</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="cend.0o">
  <kap><tld/>o</kap>
  <snc mrk="cend.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
    <ref tip="lst" cel="unu.monasub0o" lst="voko:monunuoj">Mona
    subunuo</ref>, egala al
    centono de
    <ref tip="malprt" cel="dolar.0o">dolaro</ref>,
    <ref tip="malprt" cel="euxr.0o">e&ubreve;ro</ref>,
    <ref tip="malprt" cel="gulden.0o">guldeno</ref>;
    tiomvalora <ref tip="super" cel="mon.0ero">monero</ref>:
      <ekz>
      kiam li revenis, li metis mil du<tld/>ajn po&scirc;tmarkojn
      sur la pupitron
        <fnt>
          <aut>J. London, trad. D. Kaiser, F Vohla, H.G. Kaiser</aut>,
          <vrk>La meksikano</vrk>,
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_e;&c_n;&c_t;</trd>
  <trd lng="ca">c&egrave;ntim</trd>
  <trdgrp lng="de">
     <trd>Cent</trd>,
     <trd>Eurocent</trd>
  </trdgrp>
  <trd lng="el">&lambda;&epsilon;&pi;&tau;&omikron_ton;</trd>
  <trd lng="en">cent</trd>
  <trd lng="es">centavo</trd>
  <trd lng="fa">&a_s;&a_n;&a_t;<klr tip="ind">
  (&a_z;&f_y;&a_r;&a_w;&a_A;&a_H;&a_d;
  &f_p;&a_w;&a_l;)</klr></trd>
  <trdgrp lng="fr">
    <trd>cent<klr tip="ind"> (centi&egrave;me de dollar, etc.)</klr>
    </trd>,
    <trd>centime<klr tip="amb"> (d'euro)</klr></trd></trdgrp>
  <trd lng="hu">cent</trd>
  <trd lng="it">centesimo</trd>
  <trd lng="nl">cent</trd>
  <trd lng="pl">cent</trd>
  <trd lng="pt">centavo</trd>
  <trd lng="ru">&c_c;&c_je;&c_n;&c_t;</trd>
  <trd lng="sk">cent</trd>
</drv>
</art>
