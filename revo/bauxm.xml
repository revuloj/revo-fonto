  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_n;&c_a;&c_v;&c_ib;&c_c;&c_c;&c_a; &c_n;&c_a; &c_d;&c_y;&c_b;&c_k;&c_ib;</trd>,
