  </trdgrp>
  <trd lng="de">sich <ind>aufb&auml;umen</ind></trd>
  <trd lng="en">buck<klr tip="ind"> (as a horse)</klr></trd>
  <trd lng="fr">se <ind>cabrer</ind><klr tip="ind"> (animal)</klr></trd>
  <trd lng="hu">&aacute;gaskodik<klr tip="ind"> (l&oacute;)</klr></trd>
  <trd lng="ru">&c_v;&c_s;&c_t;&c_a;&c_v;&c_a;&c_t;&c_mol; &c_n;&c_a; &c_d;&c_y;&c_b;&c_y;</trd>
</drv>
