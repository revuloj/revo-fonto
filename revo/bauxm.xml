<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bauxm.xml,v 1.20 2023/12/05 08:31:39 revo Exp $">
<kap>
  <rad>ba&ubreve;m</rad>/i <fnt><bib>KrED</bib></fnt>
</kap>
<drv mrk="bauxm.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="bauxm.0i.RAJ">
    <dif>
      <ref tip="super" cel="star.0igxi.rektigxi">Stari&gcirc;i</ref> sur la postaj piedoj,
      kiel faras &ccirc;evalo a&ubreve; simila kvarpiedulo:
      <ekz>
        la &ccirc;evalo de la oficiro <tld/>is
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de
        Diego</aut>, <vrk>Cent
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>;
      </ekz>
      <ekz>
        la sova&gcirc;a bovo stampfas, prancas, <tld/>as, provas &ccirc;iel skufaligi la vireton
        <fnt>
          <bib>KrB</bib>
          <lok>Ripetado estas plej bona lernado</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        falegis <klr>[&#8230;]</klr> ni &ccirc;iuj, dum la sanga perfido <tld/>is venke
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="kalcitr.0i">kalcitri</ref>
    <refgrp tip="vid">
      <ref cel="karako.0i">karakoli</ref>,
      <ref cel="pranc.0i.RAJ">pranci<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_s;&c_t;&c_a;&c_n;&c_a;&c_v;&c_ib;&c_c;&c_c;&c_a;
      &c_n;&c_a; &c_d;&c_y;&c_b;&c_k;&c_ib;</trd>,
      <trd>&c_s;&c_t;&c_a;&c_n;&c_a;&c_v;&c_ib;&c_c;&c_c;&c_a;
      &c_d;&c_y;&c_b;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="de">sich <ind>aufb&auml;umen</ind></trd>
    <trd lng="en">to rear<klr tip="ind"> (as a horse)</klr></trd>
    <trd lng="fr">se <ind>cabrer</ind><klr tip="ind">
    (animal)</klr></trd>
    <trd lng="hu">&aacute;gaskodik<klr tip="ind">
    (l&oacute;)</klr></trd>
    <trdgrp lng="pl">
      <trd>stawa&cacute; d&eogonek;ba<klr> (np. o koniu)</klr></trd>,
      <trd>wspina&cacute; si&eogonek; na tylne (zadnie) nogi</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_s;&c_t;&c_a;&c_v;&c_a;&c_t;&c_mol; &c_n;&c_a;
    &c_d;&c_y;&c_b;&c_y;</trd>
  </snc>
  <snc mrk="bauxm.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <uzo tip="stl">POE</uzo>
    <dif>
      <ref tip="super" cel="lev.0igxi.altigxi">Levi&gcirc;i</ref> eletendante sin
      alsupre:
      <ekz>
        la malkvietaj ondoj <tld/>as &ccirc;i tie, ili alti&gcirc;as al
        la kvieta kaj malvarma &ccirc;ielo
        <fnt>
          <aut>E. Kumi&ccaron;i&cacute;, trad. S. Rukelj</aut>,
          <vrk><url ref="http://free-vz.t-com.hr/Stanko/Surprizitaj.pdf">
          Surprizitaj nuptofestantoj</url></vrk>,
          <lok>inko, 2001</lok>
        </fnt>;
      </ekz>
      <ekz>
        libroj <klr>[&#8230;]</klr> premis sin, jen sole, jen are, <klr>[&#8230;]</klr> <tld/>ante
        &ccirc;iam pli alten
        <fnt>
          <bib>LOdE</bib>
          <aut>Pjotr Gnedi&ccirc;</aut>
          <vrk><url ref="https://esperanto-ondo.ru/Rn-gnedi.htm">Libra polvo (ombroj de l'
          pasinteco)</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_u;&c_z;&c_d;&c_y;&c_m;&c_a;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_v;&c_y;&c_r;&c_a;&c_s;&c_t;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
  <trdgrp lng="de">
    <trd>sich <ind>auft&uuml;rmen</ind></trd>,
    <trd>sich <ind>t&uuml;rmen</ind></trd>,
    <trd>aufstreben</trd>,
    <trd>aufragen</trd>
  </trdgrp>
  </snc>
  <trd lng="he">&lamed;&he;&zayin;&dalet;&qof;&fpe; (&kaf;&mem;&vav;
  &samekh;&vav;&samekh;)</trd>
  <trdgrp lng="pl">
    <trd>wspina&cacute; si&eogonek;</trd>,
    <trd>wyrasta&cacute;</trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_s;&c_t;&c_a;&c_v;&c_a;&c_t;&c_i; (&c_a;&c_b;&c_o;&nbsp;&c_z;&c_v;&c_o;&c_d;&c_i;&c_t;&c_i;&c_s;&c_ja;) &c_n;&c_a; &c_d;&c_i;&c_b;&c_i;&nbsp;(&c_p;&c_r;&c_o;
  &c_k;&c_o;&c_n;&c_ja;)</trd>
  <trdgrp lng="zh">
    <trd>&#x53CD;&#25239;&#32773; <pr>f&#x1CE;nk&#xE0;ngzh&#x11B;</pr></trd>,
    <trd>&#x53DB; <pr>p&#xE0;n</pr></trd>
  </trdgrp>
</drv>
</art>
