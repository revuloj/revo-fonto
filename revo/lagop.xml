<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>lagop</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="lagop.0o">
  <kap><tld/>o</kap>
  <snc mrk="lagop.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super"
      cel="tetraon1.0oj">tetraonedoj</ref> (<trd
      lng="la">Lagopus</trd>),
      koke grandaj birdoj vivantaj tre norde en Ameriko kaj E&ubreve;razio,
      aliloke en altaj montaroj, ofte &ccirc;asataj:
      <ekz>
        ermenoj, leporoj kaj <tld/>oj <klr>...</klr> &scirc;an&gcirc;as siajn
        kolorojn por adapti&gcirc;i al la la&ubreve;sezone &scirc;an&gcirc;i&gcirc;anta medio
        <fnt>
        <aut>Saliko</aut>,
        <vrk><url
        ref="http://www.esperanto.be/fel/2005/008395.php">Vizito
        al la nacia parko Vanoise</url></vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2005a, numero 2a, p. 22a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Schneehuhn</trd>
  <trd lng="fr">lagop&egrave;de</trd>
  <trd lng="hu">h&oacute;fajd</trd> 
  <trd lng="nl">sneeuwhoen</trd> 
</drv>
</art>

