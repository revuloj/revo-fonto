<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>turnir</rad>/o
</kap>
<drv mrk="turnir.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;e la mezepoko, lukto de kavaliroj en publika festa vetbatalo.
    </dif>
    <trd lng="hu">lovagi <ind>torna</ind></trd>    
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Granda publika konkurso:
      <ekz>
        la trubaduraj <tld/>oj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind>&scirc;ak<tld/></ind>o.
        <trd lng="hu">sakktorna</trd>
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>torna</trd>,
      <trd>verseny</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_r;&c_n;&c_ib;&c_r;</trd>
  <trd lng="de">Turnier</trd>
  <trd lng="fr">tournoi</trd>
  <trd lng="nl">toernooi</trd>
  <trd lng="ru">&c_t;&c_u;&c_r;&c_n;&c_i;&c_r;</trd>
</drv>
</art>
