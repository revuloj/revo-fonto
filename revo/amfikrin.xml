<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: amfikrin.xml,v 1.6 2022/04/25 15:10:59 revo Exp $">
<kap>
    <rad>amfikrin</rad>/a
    <fnt>
      <aut>Vikipedio</aut>,
      <vrk><url
      ref="&Viki;/Glando">Glando</url></vrk>
    </fnt>
</kap>
<drv mrk="amfikrin.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">FIZL</uzo>
  <!-- nebezone distingi KOMUNE kaj FIZL, unue trovu ekz-ojn, kie distingo necesas
  <snc mrk="amfikrin.0a.KOMUNE">
  <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Rilata al la <tld/>aj <ref tip="malprt"
      cel="gland.0o">glandoj</ref>.
    </dif>
  </snc>
  -->
  <snc mrk="amfikrin.0a.FIZL">
  <uzo tip="klr">(p.p. glando)</uzo>
    <dif>
      Kiu estas samtempe <ref tip="vid"
      cel="endokrin.0a.FIZL">endokrina</ref> kaj <ref tip="vid"
      cel="ekzokrin.0a.FIZL">ekzokrina</ref>, jen &ccirc;ar &gcirc;i enhavas amba&ubreve;
      tipojn da &ccirc;eloj (ekz. pankreato, gonadoj), jen &ccirc;ar &gcirc;iaj &ccirc;eloj
      havas
      amba&ubreve; funkciojn (ekz. hepato).
    </dif>
  </snc>
  <trd lng="en">amphicrine</trd>
  <trd lng="fr">amphicrine</trd>
</drv>
</art>

