<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>samo</rad>/o
</kap>

<drv mrk="samo.0o">
  <kap>
    <tld lit="S"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="samo.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Insularo en la <ref tip="vid" cel="pacifik.0o">Pacifiko</ref>.
      Teritorio de &scirc;tato Samoo.
    </dif>
    <trdgrp lng="pl">
      <trd>Wyspy Samoa</trd>
    </trdgrp>
  </snc>
  <snc mrk="samo.0o.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      &Scirc;tato en la samnoma insularo kun &ccirc;efurbo Apio.
      ISO-3166 kodo: WS.
    </dif>
    <trdgrp lng="pl">
      <trd>Niezale&zdot;ne Pa&nacute;stwo <ind>Samoa</ind></trd>,
      <trd>Samoa Zachodnie<klr> (przest.)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="bg">&c_S;&c_a;&c_m;&c_o;&c_a;</trd>
  <trd lng="de">Samoa</trd>
  <trd lng="en">Samoa</trd>
</drv>

<drv mrk="samo.okcidenta0o">
  <kap>
    Okcidenta <tld lit="S"/>o
    <fnt>
      <vrk>
        <url ref="http://www.esperanto.be/fel/jar/landoj.php">La landoj
        de l' tero</url>
      </vrk>
   </fnt>
  </kap>
  <snc mrk="samo.okcidenta0o.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <uzo tip="stl">ARK</uzo>
    <dif>
      Iama nomo de <ref tip="dif" cel="samo.0o.sxtato">Samoo</ref>.
    </dif>
    <trd lng="de">Westsamoa</trd>
    <trdgrp lng="pl">
      <trd>Samoa Zachodnie<klr> (przest.)</klr></trd>
    </trdgrp>
  </snc>
</drv>

</art>

