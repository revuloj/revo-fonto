<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
    <art mrk="$Id$">
      <kap><ofc>*</ofc>
        <rad>permes</rad>/i</kap>

      <drv mrk="permes.0i">
        <kap><ofc>*</ofc><tld/>i</kap>
        <gra><vspec>tr</vspec></gra>
        <snc mrk="permes.0i.iu">
	<uzo tip="klr">(iu)</uzo>
	<dif>
	  Konsenti, ke iu libere faru iun:
	  <ekz>
	    <tld/>u al mi kisi vian manon<fnt>Z</fnt>;
	  </ekz><ekz>
	    <tld/>u al malri&ccirc;a homo, ke li ion petu de vi<fnt>Z</fnt>;
	  </ekz><ekz>
	    ricevi, atingi la <tld/>on<fnt>Z</fnt>;
	  </ekz><ekz>
	    ripoz<tld/>o.
	  </ekz>
	</dif>
	<refgrp tip="sin">
	  <ref cel="akcept.0i">akcepti</ref>,
	  <ref cel="las.al0i">allasi</ref>,
	  <ref cel="toler.0i">toleri</ref>.
	</refgrp>
	<trd lng="ru">&c_r;&c_a;&c_z;&c_r;&c_je;&c_sh;&c_i;&c_t;&c_mol;</trd>
        </snc>
        <snc mrk="permes.0i.io">
	<uzo tip="klr">(io)</uzo>
	<dif>
	  Ebligi, ke iu faru ion:
	  <ekz>
	    kiom la cirkonstancoj tion <tld/>is, li penis esti
	    eleganta<fnt>Z</fnt>;
	  </ekz><ekz>
	    la regularo <tld/>as, ke ...u<fnt>Z</fnt>;
	  </ekz><ekz>
	    la sorto ne <tld/>is al ili geedzi&gcirc;i<fnt>Z</fnt>.
	  </ekz>
	</dif>
	<trd lng="he">&lamed;<ind>&alef;&pe;&shin;&resh;</ind></trd>
	<trdgrp lng="ru">
	  <trd>&c_p;&c_o;&c_z;&c_v;&c_o;&c_l;&c_i;&c_t;&c_mol;</trd>,
	  <trd>&c_d;&c_o;&c_p;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
	</trdgrp>
        </snc>
        <trd lng="cs">dovolit</trd>
        <trdgrp lng="fr"><trd>permettre</trd><trd>autoriser</trd></trdgrp>
        <trdgrp lng="de">
	<trd>erlauben</trd>,
	<trd>gestatten</trd>
        </trdgrp>
        <trd lng="en">permit</trd>
        <trd lng="he">&lamed;&he;&resh;&shin;&vav;&tav;
\<ind>&resh;&shin;&he;</ind>\</trd>
        <trdgrp lng="hu">
	<trd>enged</trd>,
	<trd>megenged</trd>
        </trdgrp>
        <trd lng="nl">toestaan</trd>
      </drv>

      <drv mrk="permes.for0i">
        <kap><ofc>*</ofc>for<tld/>i</kap>
        <gra><vspec>tr</vspec></gra>
        <snc>
	<dif>
	  <tld lit="P"/>i al iu foriri:
	  <ekz>
	    li for<tld/>is la vizitantojn;
	  </ekz><ekz>
	    peti for<tld/>on &gcirc;is la vespero.
	  </ekz>
	</dif>
        </snc>
        <snc>
	<dif>
	  <tld lit="P"/>i al oficisto a&ubreve; servisto forlasi sian servon
	  por difinita tempo:
	  <ekz>
	    li petis for<tld/>on por &ccirc;eesti la edzi&gcirc;on de sia frato.
	  </ekz>
	</dif>
	<refgrp tip="vid">
	  <ref cel="temp.liber0o">libertempo</ref>.
	</refgrp>
        </snc>
        <trd lng="he">&lamed;<ind>&shin;&het;&resh;&resh;</ind></trd>
        <trd lng="hu">elenged</trd>
        <trd lng="nl">vrijaf geven</trd>
        <trd lng="fr">cong&eacute;dier</trd>
        <trd lng="ru">&c_o;&c_t;&c_p;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
      </drv>

      <drv mrk="permes.mal0i">
        <kap>mal<tld/>i</kap>
        <gra><vspec>tr</vspec></gra>
        <snc>
	<dif>
	  Ne konsenti, ke iu faru ion:
	  <ekz>
	    malobei, transpa&scirc;i mal<tld/>on;
	  </ekz><ekz>
	    frukto mal<tld/>ita estas plej bongusta
	    <fnt><bib>PrV</bib></fnt>.
	  </ekz>
	</dif>
        </snc>
        <trdgrp lng="de">
            <trd>verbieten</trd>,
            <trd>untersagen</trd>
        </trdgrp>
        <trd lng="he">&lamed;&alef;&samekh;&vav;&resh;
\<ind>&alef;&samekh;&resh;</ind>\<klr tip="ind"> (&lamed;&alef;
&lamed;&he;&resh;&shin;&vav;&tav;)</klr></trd>
        <trd lng="hu">megtilt</trd>
        <trd lng="nl">verbieden</trd>
        <trdgrp
lng="fr"><trd>interdire</trd><trd>d&eacute;fendre</trd></trdgrp>
        <trd lng="ru">&c_z;&c_a;&c_p;&c_r;&c_je;&c_t;&c_i;&c_t;&c_mol;</trd>
      </drv>

      <drv mrk="permes.0ilo">
        <kap><tld/>ilo</kap>
        <snc>
	<dif>
	  Dokumento a&ubreve; signo atestanta ke al ties posedanto estas
	  <tld/>ite fari ion:
	  <ekz>
	    <uzo tip="fak">AUT</uzo>
	    <ind>stir<tld/>ilo</ind>, konduk<tld/>ilo;
	    <trd lng="en">driver's lincense</trd>
	    <trd lng="fr">permis de conduire</trd>
	    <trd lng="he">&resh;&shin;&yod;&vav;&fnun;
&nun;&he;&yod;&gimel;&he;</trd>
	    <trd lng="hu">vezet&odblac;i <ind>jogos&iacute;tv&aacute;ny</ind></trd>
	    <trd lng="nl">rijbewijs</trd>
	    <trd
lng="ru">&c_v;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_k;&c_i;&c_je;
&c_p;&c_r;&c_a;&c_v;&c_a;</trd>
	  </ekz><ekz>
	    <uzo tip="fak">KOMP</uzo>
	    <ind>GNUa &Gcirc;enerala Publika <tld lit="P"/>ilo</ind>;
	    <trd lng="bg">&c_O;&c_b;&c_shch;&c_o;&c_t;&c_o;
&c_P;&c_r;&c_a;&c_v;&c_o; &c_n;&c_a;
&c_O;&c_b;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_o;
	      &c_P;&c_o;&c_l;&c_z;&c_v;&c_a;&c_n;&c_je; &c_G;&c_N;&c_U;</trd>
	    <trd lng="en">GNU General Public License</trd>
	    <trd lng="fr">Licence Publique G&eacute;n&eacute;rale GNU</trd>
	    <trd
lng="ru">&c_S;&c_t;&c_a;&c_n;&c_d;&c_a;&c_r;&c_t;&c_n;&c_a;&c_ja;
&c_O;&c_b;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_a;&c_ja;
	      &c_L;&c_i;&c_c;&c_je;&c_n;&c_z;&c_i;&c_ja; GNU</trd>
	  </ekz><ekz>
	    brita privata <ind>pilota <tld/>ilo</ind>.
	    <fnt>
	      <aut>Hju Rid (Hugh Reid)</aut>:
	      <lok><url ref="http://www.piloto.u-net.com/pages/esp5.htm"
		  >Aviado</url>.</lok>
	    </fnt>
	    <trd lng="en">pilot's licence</trd>
	    <trd lng="he">&resh;&shin;&yod;&vav;&fnun;
&tet;&yod;&yod;&samekh;</trd>
	    <trd lng="nl">vliegbrevet</trd>
	    <trd lng="ru">&c_p;&c_i;&c_l;&c_o;&c_t;&c_a;&c_zh;&c_n;&c_o;&c_je;
&c_s;&c_v;&c_i;&c_d;&c_je;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>
	  </ekz>
	</dif>
        </snc>
        <trd lng="en">license</trd>
        <trd lng="he">&resh;&shin;&yod;&vav;&fnun;</trd>
        <trdgrp lng="hu">
	<trd>enged&eacute;ly</trd>,
	<trd>igazolv&aacute;ny</trd>
        </trdgrp>
        <trd lng="nl">brevet</trd>
        <trdgrp lng="ru">
	<trd>&c_r;&c_a;&c_z;&c_r;&c_je;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>,
	<trd>&c_l;&c_i;&c_c;&c_je;&c_n;&c_z;&c_i;&c_ja;</trd>
        </trdgrp>
      </drv>
    </art>
