</kap>
<drv mrk="permes.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="permes.0i.iu">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Konsenti, ke iu libere faru ion:
      <ekz>
        <tld/>u al mi kisi vian manon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>u al malri&ccirc;a homo, ke li ion petu de vi<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ricevi, atingi la <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ripoz<tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akcept.0i">akcepti</ref>,
      <ref cel="las.al0i">allasi</ref>,
      <ref cel="toler.0i">toleri</ref>
    </refgrp>
    <trd lng="br">aotren</trd>
    <trdgrp lng="de">
      <trd>erlauben</trd>,
      <trd>gestatten</trd>
    </trdgrp>    
    <trd lng="ru">&c_r;&c_a;&c_z;&c_r;&c_je;&c_sh;&c_i;&c_t;&c_mol;</trd>
    <trd lng="th">&#3629;&#3609;&#3640;&#3597;&#3634;&#3605;</trd>
  </snc>
  <snc mrk="permes.0i.io">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Ne malebligi, ke iu faru ion;
      allasi, ke io okazu:
      <ekz>
        kiom la cirkonstancoj tion <tld/>is, li penis esti
        eleganta<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la regularo <tld/>as, ke ...u<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la sorto ne <tld/>is al ili geedzi&gcirc;i<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="br">rei&ntilde; an <ind>tu</ind> da</trd>
    <trdgrp lng="de">
      <trd>geschehen lassen</trd>,
      <trd>zulassen</trd>
    </trdgrp>
    <trd lng="he">&lamed;<ind>&alef;&pe;&shin;&resh;</ind></trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_z;&c_v;&c_o;&c_l;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_d;&c_o;&c_p;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_z;&c_v;&c_a;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_p;&c_u;&c_s;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd
  lng="bg">&c_p;&c_o;&c_z;&c_v;&c_o;&c_l;&c_ja;&c_v;&c_a;&c_m;</trd>
  <trd lng="br">permeti&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>dovolit</trd>,
    <trd>dovolovat</trd>,
    <trd>povolit</trd>,
    <trd>svolit</trd>,
    <trd>umo&zcaron;nit</trd>
  </trdgrp>
  <trdgrp lng="en">
  <trd>allow</trd>,
  <trd>permit</trd>
</trdgrp>
  <trdgrp lng="es">
  <trd>permitir</trd>,
  <trd>autorizar</trd>
</trdgrp>
  <trdgrp lng="fa">
    <trd>&a_A;&a_j;&a_A;&a_z;&a_h; &a_d;&a_A;&a_d;&a_n;</trd>,
    <trd>&a_r;&a_w;&a_A; &a_d;&a_A;&a_sh;&a_t;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>autoriser</trd>,
    <trd>permettre</trd>
  </trdgrp>
  <trd lng="he">&lamed;&he;&resh;&shin;&vav;&tav;
  \<ind>&resh;&shin;&he;</ind>\</trd>
  <trdgrp lng="hu">
    <trd>enged</trd>,
    <trd>megenged</trd>
  </trdgrp>
  <trd lng="it">permettere</trd>
  <trd lng="nl">toestaan</trd>
  <trd lng="pt">permitir</trd>
  <trdgrp lng="sk">
    <trd>dopusti&tcaron;</trd>,
    <trd>dovoli&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">till&aring;ta</trd>
  <trd lng="tp">ken</trd>
  <trd lng="tr">izin vermek</trd>
</drv>

<drv mrk="permes.for0i">
  <kap><ofc>*</ofc>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="permes.for0i.KOMUNE">
    <dif>
      <tld lit="P"/>i al iu foriri:
      <ekz>
        li for<tld/>is la vizitantojn;
      </ekz>
      <ekz>
        peti for<tld/>on &gcirc;is la vespero.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      <tld lit="P"/>i al oficisto a&ubreve; servisto forlasi sian servon
      por difinita tempo:
      <ekz>
        li petis for<tld/>on por &ccirc;eesti la edzi&gcirc;on de sia
        frato.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="temp.liber0o">libertempo</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>beurlauben</trd>,
      <trd>freistellen <klr>(von einem Arbeitsverh&auml;ltnis)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_p;&c_u;&c_s;&c_k;&c_a;&c_c;&c_mol;</trd>
  <trd lng="br">digouvia&ntilde;</trd>
  <trd lng="cs">d&aacute;t volno</trd>
  <trd lng="en">dismiss</trd>
  <trd lng="fa">&a_m;&a_r;&a_kh;&a_S; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">cong&eacute;dier</trd>
  <trd lng="he">&lamed;<ind>&shin;&het;&resh;&resh;</ind></trd>
  <trd lng="hu">elenged</trd>
  <trdgrp lng="it">
    <trd>licenziare<klr> (permettere di assentarsi)</klr></trd>,
    <trd>congedare<klr> (permettere di assentarsi)</klr></trd>
  </trdgrp>
  <trd lng="nl">vrijaf geven</trd>
  <trd lng="ru">&c_o;&c_t;&c_p;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">da&tcaron; vo&lcaron;no</trd>
</drv>

<drv mrk="permes.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="permes.mal0i.KOMUNE">
    <dif>
      Ne konsenti, ke iu faru ion:
      <ekz>
        malobei, transpa&scirc;i mal<tld/>on;
      </ekz>
      <ekz>
        frukto mal<tld/>ita estas plej bongusta
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd
  lng="be">&c_z;&c_a;&c_b;&c_a;&c_r;&c_a;&c_n;&c_ja;&c_c;&c_mol;</trd>
  <trd
  lng="bg">&c_z;&c_a;&c_b;&c_r;&c_a;&c_n;&c_ja;&c_v;&c_a;&c_m;</trd>
  <trdgrp lng="br">
    <trd>berza&ntilde;</trd>,
    <trd>difenn<klr tip="ind"> (berza&ntilde;)</klr></trd>,
    <trd>mirout<klr tip="ind"> (berza&ntilde;)</klr></trd>
  </trdgrp>
  <trd lng="cs">zak&aacute;zat</trd>
  <trdgrp lng="de">
    <trd>verbieten</trd>,
    <trd>untersagen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>prohibit</trd>,
    <trd>forbid</trd>,
    <trd>disallow</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>prohibir</trd>,
    <trd>vedar</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_m;&a_n;&a_ayn; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_m;&a_m;&a_n;&a_w;&a_ayn; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_q;&a_d;&a_gh;&a_n; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>interdire</trd>,
    <trd>d&eacute;fendre</trd>
  </trdgrp>
  <trd lng="he">&lamed;&alef;&samekh;&vav;&resh;
  \<ind>&alef;&samekh;&resh;</ind>\<klr tip="ind"> (&lamed;&alef;
  &lamed;&he;&resh;&shin;&vav;&tav;)</klr></trd>
  <trd lng="hu">megtilt</trd>
  <trdgrp lng="it">
    <trd>proibire</trd>,
    <trd>vietare</trd>
  </trdgrp>
  <trd lng="nl">verbieden</trd>
  <trd lng="ru">&c_z;&c_a;&c_p;&c_r;&c_je;&c_t;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>nedovoli&tcaron;</trd>,
    <trd>nepovoli&tcaron;</trd>,
    <trd>zak&aacute;za&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">f&ouml;rbjuda</trd>
  <trd lng="tr">yasaklamak</trd>
</drv>

<drv mrk="permes.0o">
  <kap><tld/>o</kap>
  <snc mrk="permes.0o.KOMUNE">
    <dif>
      Ago, vortoj de tiu, kiu <tld/>as:
      <ekz>
  Peti la <tld/>on fari ion.
      </ekz>
      </dif>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_r;&c_a;&c_z;&c_r;&c_je;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_u;&c_p;&c_malmol;&c_l;&c_n;&c_o;&c_m;&c_o;&c_shch;&c_a;&c_v;&c_a;&c_n;&c_je;<klr
