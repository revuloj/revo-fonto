<drv mrk="permes.konduk0ilo">
  <kap>konduk<tld/>ilo, <var><kap>stir<tld/>ilo</kap></var></kap>
  <snc mrk="permes.konduk0ilo.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      <ref tip="super" cel="permes.0ilo.KOMUNE"><tld lit="P"/>ilo</ref> 
      pri kondukado de veturilo:
      <ekz>
        konduk<tld/>iloj por a&ubreve;tomobilo kaj boato
        <fnt>
          <aut>N. Gaufillet</aut>,
          <vrk><url ref="http://www.eklaboru.com/resumo_priskribo.php?id=36">
          Elturni&gcirc;ulo</url></vrk>,
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        a&ubreve;tomobila stir<tld/>ilo estas postulata por a&ubreve;tovoja&gcirc;antoj
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="&Monato;/2014/011323.php">Bicikle
          al Nord-Koreio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ni luis a&ubreve;ton, por kio ni bezonis stir<tld/>ilon de la
        polico
        <fnt>
          <aut>J. Fox</aut>,
          <vrk>Jackie kaj Brian Fox Ferias...</vrk>,
          <lok><url ref="http://www.esperanto.org.nz/af/ag-filiko-22.html">
          Ar&gcirc;enta Filiko, 2006-09, numero 22a</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        renovigi sian stir<tld/>ilon
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2013/011031.php">Hundo &ccirc;e
          la fundo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="th">&#3651;&#3610;&#3629;&#3609;&#3640;&#3597;&#3634;&#3605;&#3586;&#3633;&#3610;&#3586;&#3637;&#3656;</trd>
  </snc>
  <trdgrp lng="de">
