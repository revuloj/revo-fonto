  </trdgrp>
  <trdgrp lng="cs">
    <trd>dovolen&iacute;</trd>,
    <trd>povolen&iacute;</trd>
  </trdgrp>
  <trd lng="de">Erlaubnis <klr>(Handlung, Zustimmung)</klr></trd>
  <trd lng="en">permission</trd>
  <trdgrp lng="es">
    <trd>permiso</trd>,
    <trd>autorizaci&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>permission</trd>,
