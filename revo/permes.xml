<drv mrk="permes.for0i">
  <kap><ofc>*</ofc>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="permes.for0i.KOMUNE">
    <dif>
      <tld lit="P"/>i al iu foriri:
      <ekz>
        li for<tld/>is la vizitantojn;
      </ekz>
      <ekz>
        peti for<tld/>on &gcirc;is la vespero.
      </ekz>
    </dif>
    <trd lng="es">permiso<klr> (permitir salir)</klr></trd>
  </snc>
  <snc>
    <dif>
      <tld lit="P"/>i al oficisto a&ubreve; servisto forlasi sian servon
      por difinita tempo:
      <ekz>
        li petis for<tld/>on por &ccirc;eesti la edzi&gcirc;on de sia
        frato.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="temp.liber0o">libertempo</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>beurlauben</trd>,
      <trd>freistellen <klr>(von einem Arbeitsverh&auml;ltnis)</klr></trd>
    </trdgrp>
    <trd lng="es">permiso<klr> (permitir ausentarse)</klr></trd>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_p;&c_u;&c_s;&c_k;&c_a;&c_c;&c_mol;</trd>
  <trd lng="br">digouvia&ntilde;</trd>
