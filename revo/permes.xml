<drv mrk="permes.konduk0ilo">
  <kap>konduk<tld/>ilo, <var><kap>stir<tld/>ilo</kap></var></kap>
  <snc mrk="permes.konduk0ilo.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      <ref tip="super" cel="permes.0ilo.KOMUNE"><tld lit="P"/>ilo</ref> 
      pri kondukado de veturilo:
      <ekz>
        konduk<tld/>iloj por a&ubreve;tomobilo kaj boato
        <fnt>
          <aut>N. Gaufillet</aut>,
          <vrk><url
          ref="http://www.eklaboru.com/resumo_priskribo.php?id=36">
          Elturni&gcirc;ulo</url></vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
      <ekz>
        ni luis a&ubreve;ton, por kio ni bezonis stir<tld/>ilon de la
        polico
        <fnt>
          <aut>J. Fox</aut>,
          <vrk>Jackie kaj Brian Fox Ferias...</vrk>,
          <lok><url
          ref="http://www.esperanto.org.nz/af/ag-filiko-22.html">
          Ar&gcirc;enta Filiko, 2006-09, numero 22a</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="th">&#3651;&#3610;&#3629;&#3609;&#3640;&#3597;&#3634;&#3605;&#3586;&#3633;&#3610;&#3586;&#3637;&#3656;</trd>
  </snc>
  <trdgrp lng="de">
    <trd>F&uuml;hrerschein<klr> (de)</klr></trd>,
    <trd>Fahrausweis<klr> (at)</klr></trd>
  </trdgrp>
  <trd lng="br">aotre-blenia&ntilde;</trd>
  <trd lng="en">driver's license</trd>
  <trd
  lng="fa">&f_g;&a_w;&a_A;&a_h;&f_y;&a_n;&a_A;&a_m;&a_h;&a_hamza_sure;
  &a_r;&a_A;&a_n;&a_n;&a_d;&f_g;&f_y;</trd>
  <trd lng="fr">permis de conduire</trd>
  <trd lng="he">&resh;&shin;&yod;&vav;&fnun;
  &nun;&he;&yod;&#8503;&he;</trd>
  <trd lng="hu">vezet&odblac;i
  <ind>jogos&iacute;tv&aacute;ny</ind></trd>
  <trd lng="it">patente<klr> di guida</klr></trd>
  <trd lng="nl">rijbewijs</trd>
  <trd
  lng="ru">&c_v;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_k;&c_i;&c_je;
  &c_p;&c_r;&c_a;&c_v;&c_a;</trd>
  <trdgrp lng="sv">
    <trd>k&ouml;rkort</trd>,
    <trd>f&ouml;rarbevis</trd>
  </trdgrp>
  <trd lng="tr">araba ehliyeti</trd>
</drv>
