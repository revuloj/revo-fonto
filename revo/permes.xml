    <trd>difenn<klr tip="ind"> (berza&ntilde;)</klr></trd>,
    <trd>mirout<klr tip="ind"> (berza&ntilde;)</klr></trd>
  </trdgrp>
  <trd lng="cs">zak&aacute;zat</trd>
  <trdgrp lng="de">
    <trd>verbieten</trd>,
    <trd>untersagen</trd>
  </trdgrp>
  <trdgrp lng="en">
