<drv mrk="permes.0o">
  <kap><tld/>o</kap>
  <snc mrk="permes.0o.KOMUNE">
    <dif>
      Ago, vortoj de tiu, kiu <tld/>as:
      <ekz>
        kiu donis al vi la <tld/>on konstrui &ccirc;i tiun domon
        <fnt>
          <bib>MT</bib>
          <lok>Ezra 5:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili venigu cedrojn de Lebanon per la maro &gcirc;is Jafo, konforme al la <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Ezra 3:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        la akuzito <klr>[&#8230;]</klr> ricevos <tld/>on defendi sin pri la akuzo
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 25:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        li perdis la vojon, kaj petis la <tld/>on tranokti &ccirc;e li
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        li eldevigis la <tld/>on mian
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>kun la <tld/>o</ind> de via ekscelenco, kion mi povis fari
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>?
        <trd lng="de">mit <ind>Verlaub</ind>...</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_z;&c_v;&c_o;&c_l;</trd>
  <trdgrp lng="bg">
    <trd>&c_r;&c_a;&c_z;&c_r;&c_je;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_u;&c_p;&c_malmol;&c_l;&c_n;&c_o;&c_m;&c_o;&c_shch;&c_a;&c_v;&c_a;&c_n;&c_je;<klr tip="amb"> (oficiale)</klr></trd>
