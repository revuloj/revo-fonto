<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kolaps.xml,v 1.23 2025/10/26 08:00:21 revo Exp $">
<kap>
  <rad>kolaps</rad>/o
  <fnt><bib>PVEF</bib></fnt>
</kap>

<drv mrk="kolaps.0o">
  <kap><tld/>o</kap>
  <snc mrk="kolaps.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">Malsano</ref> karakterizata per
      sensinkopa,
      sed rapida malplii&gcirc;o de fortoj a&ubreve; funkcioj:
      <ekz>
        mi ne imagas lin venkebla de tiaj fizikaj <tld/>oj
        <fnt>
          <bib>ChB</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="sinkop.0o.sveno">sinkopo<sncref/></ref>,
      <ref cel="sven.0o">sveno</ref>
    </refgrp>
    <trd lng="de">Kreislaufversagen</trd>
    <trd lng="fr">collapsus</trd>
  </snc>
  <snc mrk="kolaps.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <klr>(p.p. asocio, entrepreno, politika sistemo...)</klr>
      Subita nekapablo funkcii:
      <ekz>
        <klr>[tio]</klr> ne multe ta&ubreve;gis por apogi la propagandan tezon, ke kapitalismo
        staras sur la sojlo de <tld/>o
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        malnova, <tld/>preta remizo
        <fnt>
          <bib>VRA</bib>
        </fnt>;
      </ekz>
      <ekz>
        kompleta <tld/>o de komunismo en preska&ubreve; &ccirc;iuj landoj
        <fnt>
          <bib>Kon11</bib>
        </fnt>;
      </ekz>
      <ekz>
        politika &scirc;an&gcirc;i&gcirc;o kaj <tld/>o de la movadaj strukturoj
        <fnt>
          <bib>Kon11</bib>
        </fnt>;
      </ekz>
      <ekz>
        plej gravaj mediaj problemoj estas la perdo de biodiverseco, la ekosistema
        <tld/>o kaj la klimata &scirc;an&gcirc;o
        <fnt>
          <bib>Kon11</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fal.dis0o">disfalo</ref>,
      <ref cel="krasx.0o">kra&scirc;o</ref>
    </refgrp>
    <trdgrp lng="fr">
      <trd>chute</trd>,
      <trd>affaissement</trd>,
      <trd>d&eacute;faite</trd>,
      <trd>d&eacute;gringolade</trd>,
      <trd>&eacute;croulement</trd>,
      <trd>effondrement</trd>,
      <trd>faillite</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_ja;&c_p;&c_s;</trd>
  <trd lng="ca">col&middot;lapse</trd>
  <trdgrp lng="cs">
    <trd>kolaps</trd>,
    <trd>zhroucen&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kollaps</trd>,
    <trd>Zusammenbruch</trd>
  </trdgrp>
  <trd lng="en">collapse</trd>
  <trd lng="es">colapso</trd>
  <trdgrp lng="ja">
    <trd>&#34394;&#33073;  <pr>&#12365;&#12423;&#12384;&#12388;</pr></trd>,
    <trd>&#23849;&#22730;  <pr>&#12411;&#12358;&#12363;&#12356;</pr></trd>,
    <trd>&#12501;&#12522;&#12540;&#12474;</trd>,
    <trd>&#12495;&#12531;&#12464;&#12450;&#12483;&#12503;</trd>,
    <trd>&#24847;&#27671;&#28040;&#27784;  <pr>&#12356;&#12365;&#12375;&#12423;&#12358;&#12385;&#12435;</pr></trd>,
    <trd>&#34928;&#24369;  <pr>&#12377;&#12356;&#12376;&#12419;&#12367;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>instorting</trd>,
    <trd>inzinking</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kollaps</trd>,
    <trd>zapa&sacute;&cacute;</trd>
  </trdgrp>
<trdgrp lng="ro">
    <trd>c&abreve;dere</trd>,
    <trd>colaps</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_l;&c_a;&c_p;&c_s;</trd>
  <trdgrp lng="sk">
    <trd>kolaps</trd>,
    <trd>n&aacute;hle ochabnutie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_k;&c_o;&c_l;&c_a;&c_p;&c_s;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_a;&c_n;&c_n;&c_ja; &c_r;&c_ib;&c_z;&c_k;&c_o;&c_ji; &c_s;&c_l;&c_a;&c_b;&c_o;&c_s;&c_t;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#22604;&#19979; <pr>t&amacron;xi&agrave;</pr></trd>,
    <trd>&#22446; <pr>ku&#462;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kolaps.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kolaps.0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Suferi <tld/>on:
      <ekz>
        mi timis, ke mia koro <tld/>os, a&ubreve; ke mi falos en frenezon
        <fnt>
          <bib>CKv</bib>
        </fnt>;
      </ekz>
      <ekz>
        la hejtisto en la kaldronego <tld/>as kaj falas dure teren
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        miaj dikfingroj pu&scirc;is profunden en lian ha&ubreve;ton,
        kaj li <tld/>is <klr>(falis sveninte)</klr>
        <fnt><bib>&Scirc;talrato</bib><lok>&ccirc;ap. 27</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi &scirc;altis la 3V-on, ricevante la finon de la nova&jcirc;oj,
        <tld/>is en la banujon kaj a&ubreve;skultis duonorele
        <fnt><bib>&Scirc;talrato</bib><lok>&ccirc;ap. 10</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="konsci.sen0igxi">senkonscii&gcirc;i</ref>,
      <ref cel="sven.0i.PSI">sveni</ref>
    </refgrp>
  <trd lng="be">&c_b;&c_y;&c_c;&c_mol; &c_u; &c_s;&c_t;&c_a;&c_n;&c_je;
  <ind>&c_k;&c_a;&c_l;&c_ja;&c_p;&c_s;&c_u;</ind></trd>
  </snc>
  <snc mrk="kolaps.0o.FIG">
    <dif>
      Subite sisteme malfunkcii, <ref cel="fal.0i.perei">fali<sncref/></ref>:
      <ekz>
        &scirc;ia revkastelo pri certa kaj honesta ekzisto subite <tld/>is
        <fnt>
          <bib>KKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        la kaduka elektra kuirforno ne volis funkcii <klr>[&#8230;]</klr> mi sukcesis tiom
        riparadi per mia po&scirc;tran&ccirc;ilo, &gcirc;is fine &gcirc;i emis varmigi, sed tiam &gcirc;iaj
        rompi&gcirc;intaj piedoj <tld/>is
        <fnt>
          <bib>VaK</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fal.dis0i">disfali</ref>,
      <ref cel="implod.0i">implodi</ref>,
      <ref cel="krasx.0i">kra&scirc;i</ref>
    </refgrp>
  <trd lng="be">&c_k;&c_a;&c_l;&c_ja;&c_p;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </snc>
  <trdgrp lng="ca">
    <trd>patir un <ind>col&middot;lapse</ind></trd>,
    <trd>sofrir un <ind>col&middot;lapse</ind></trd>,
    <trd>col&middot;lapsar-se</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>utrp&ecaron;t kolaps</trd>,
    <trd>zhroutit se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>kollabieren</trd>,
    <trd>zusammenbrechen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&ecirc;tre victime d'un <ind>collapsus</ind></trd>,
    <trd>s'<ind>abattre</ind></trd>,
    <trd>s'<ind>affaler</ind></trd>,
    <trd>s'<ind>affaisser</ind></trd>,
    <trd>s'<ind>&eacute;crouler</ind></trd>,
    <trd>s'<ind>effondrer</ind></trd>,
    <trd>craquer</trd>,
    <trd>crouler</trd>,
    <trd>d&eacute;gringoler</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#34394;&#33073;&#12377;&#12427;  <pr>&#12365;&#12423;&#12384;&#12387;&#12377;&#12427;</pr></trd>,
    <trd>&#23849;&#22730;&#12377;&#12427;  <pr>&#12411;&#12358;&#12363;&#12356;&#12377;&#12427;</pr></trd>,
    <trd>&#12501;&#12522;&#12540;&#12474;&#12377;&#12427;</trd>,
    <trd>&#24847;&#27671;&#28040;&#27784;&#12377;&#12427;  <pr>&#12356;&#12365;&#12375;&#12423;&#12358;&#12385;&#12435;&#12377;&#12427;</pr></trd>,
    <trd>&#34928;&#24369;&#12377;&#12427;  <pr>&#12377;&#12356;&#12376;&#12419;&#12367;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="pl">zawie&sacute;&cacute;</trd>
  <trdgrp lng="ro">
    <trd>e&#537;ua</trd>,
    <trd>tr&abreve;da</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>n&aacute;hle ochabn&uacute;&tcaron;</trd>,
    <trd>utrpie&tcaron; kolaps</trd>
  </trdgrp>
  <trd lng="uk">&c_p;&c_je;&c_r;&c_je;&c_b;&c_u;&c_v;&c_a;&c_t;&c_i; &c_v; &c_k;&c_o;&c_l;&c_a;&c_p;&c_s;&c_ib;</trd>
  <trdgrp lng="zh">
    <trd>&#23849;&#28291; <pr>b&emacron;ngku&igrave;</pr></trd>,
    <trd>&#22349; <pr>t&amacron;n</pr></trd>,
    <trd>&#22604; <pr>t&amacron;</pr></trd>,
    <trd>&#20498;&#22604; <pr>d&#462;ot&amacron;</pr></trd>,
    <trd>&#22349;&#22604; <pr>t&amacron;nt&amacron;</pr></trd>,
    <trd>&#20542; <pr>q&#299;ng</pr></trd>,
    <trd>&#21719;&#21862; <pr>hu&aacute;la</pr></trd>
  </trdgrp>
</drv>
</art>
