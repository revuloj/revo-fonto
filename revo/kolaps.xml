<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kolaps.xml,v 1.21 2024/10/28 06:39:53 revo Exp $">
<kap>
  <rad>kolaps</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kolaps.0o">
  <kap><tld/>o</kap>
  <snc mrk="kolaps.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">Malsano</ref> karakterizata per
      sensinkopa,
      sed rapida malplii&gcirc;o de fortoj a&ubreve; funkcioj:
      <ekz>
        mi ne imagas lin venkebla de tiaj fizikaj <tld/>oj
        <fnt>
          <bib>ChB</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="sinkop.0o.sveno">sinkopo<sncref/></ref>,
      <ref cel="sven.0o">sveno</ref>
    </refgrp>
    <trd lng="de">Kreislaufversagen</trd>
    <trd lng="fr">collapsus</trd>
  </snc>
  <snc mrk="kolaps.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <klr>(p.p. asocio, entrepreno, politika sistemo...)</klr>
      Subita nekapablo funkcii:
      <ekz>
        <klr>[tio]</klr> ne multe ta&ubreve;gis por apogi la propagandan tezon, ke kapitalismo
        staras sur la sojlo de <tld/>o
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        malnova, <tld/>preta remizo
        <fnt>
          <bib>VRA</bib>
        </fnt>;
      </ekz>
      <ekz>
        kompleta <tld/>o de komunismo en preska&ubreve; &ccirc;iuj landoj
        <fnt>
          <bib>Kon11</bib>
        </fnt>;
      </ekz>
      <ekz>
        politika &scirc;an&gcirc;i&gcirc;o kaj <tld/>o de la movadaj strukturoj
        <fnt>
          <bib>Kon11</bib>
        </fnt>;
      </ekz>
      <ekz>
        plej gravaj mediaj problemoj estas la perdo de biodiverseco, la ekosistema
        <tld/>o kaj la klimata &scirc;an&gcirc;o
        <fnt>
          <bib>Kon11</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fal.dis0o">disfalo</ref>,
      <ref cel="krasx.0o">kra&scirc;o</ref>
    </refgrp>
    <trdgrp lng="fr">
      <trd>chute</trd>,
      <trd>affaissement</trd>,
      <trd>d&eacute;faite</trd>,
      <trd>d&eacute;gringolade</trd>,
      <trd>&eacute;croulement</trd>,
      <trd>effondrement</trd>,
      <trd>faillite</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_ja;&c_p;&c_s;</trd>
  <trdgrp lng="cs">
    <trd>kolaps</trd>,
    <trd>zhroucen&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kollaps</trd>,
    <trd>Zusammenbruch</trd>
  </trdgrp>
  <trd lng="en">collapse</trd>
  <trdgrp lng="ja">
    <trd>&#x865A;&#33073;  <pr>&#x304D;&#12423;&#12384;&#12388;</pr></trd>,
    <trd>&#x5D29;&#x58CA;  <pr>&#x307B;&#12358;&#x304B;&#12356;</pr></trd>,
    <trd>&#x30D5;&#x30EA;&#x30FC;&#x30BA;</trd>,
    <trd>&#x30CF;&#x30F3;&#x30B0;&#x30A2;&#x30C3;&#x30D7;</trd>,
    <trd>&#x610F;&#x6C17;&#x6D88;&#x6C88;  <pr>&#12356;&#x304D;&#12375;&#12423;&#12358;&#12385;&#12435;</pr></trd>,
    <trd>&#34928;&#x5F31;  <pr>&#12377;&#12356;&#12376;&#12419;&#x304F;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>instorting</trd>,
    <trd>inzinking</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kollaps</trd>,
    <trd>zapa&sacute;&cacute;</trd>
  </trdgrp>
<trdgrp lng="ro">
    <trd>c&abreve;dere</trd>,
    <trd>colaps</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_l;&c_a;&c_p;&c_s;</trd>
  <trdgrp lng="sk">
    <trd>kolaps</trd>,
    <trd>n&aacute;hle ochabnutie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&#x43B;&c_a;&#x43F;&c_s;</trd>,
    <trd>&#x43D;&c_a;&c_s;&c_t;&c_a;&#x43D;&#x43D;&#x44F; &c_r;&c_ib;&c_z;&#x43A;&#x43E;&c_ji; &c_s;&#x43B;&c_a;&c_b;&#x43E;&c_s;&c_t;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x584C;&#x4E0B; <pr>t&amacron;xi&#xE0;</pr></trd>,
    <trd>&#x57AE; <pr>ku&#x1CE;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kolaps.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kolaps.0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Suferi <tld/>on:
      <ekz>
        mi timis, ke mia koro <tld/>os, a&ubreve; ke mi falos en frenezon
        <fnt>
          <bib>CKv</bib>
        </fnt>;
      </ekz>
      <ekz>
        la hejtisto en la kaldronego <tld/>as kaj falas dure teren
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        miaj dikfingroj pu&scirc;is profunden en lian ha&ubreve;ton,
        kaj li <tld/>is <klr>(falis sveninte)</klr>
        <fnt><bib>&Scirc;talrato</bib><lok>&ccirc;ap. 27</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi &scirc;altis la 3V-on, ricevante la finon de la nova&jcirc;oj,
        <tld/>is en la banujon kaj a&ubreve;skultis duonorele
        <fnt><bib>&Scirc;talrato</bib><lok>&ccirc;ap. 10</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="konsci.sen0igxi">senkonscii&gcirc;i</ref>,
      <ref cel="sven.0i.PSI">sveni</ref>
    </refgrp>
  <trd lng="be">&c_b;&c_y;&c_c;&c_mol; &c_u; &c_s;&c_t;&c_a;&c_n;&c_je; <ind>&c_k;&c_a;&c_l;&c_ja;&c_p;&c_s;&c_u;</ind></trd>
  </snc>
  <snc mrk="kolaps.0o.FIG">
    <dif>
      Subite sisteme malfunkcii, <ref cel="fal.0i.perei">fali<sncref/></ref>:
      <ekz>
        &scirc;ia revkastelo pri certa kaj honesta ekzisto subite <tld/>is
        <fnt>
          <bib>KKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        la kaduka elektra kuirforno ne volis funkcii <klr>[&#8230;]</klr> mi sukcesis tiom
        riparadi per mia po&scirc;tran&ccirc;ilo, &gcirc;is fine &gcirc;i emis varmigi, sed tiam &gcirc;iaj
        rompi&gcirc;intaj piedoj <tld/>is
        <fnt>
          <bib>VaK</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fal.dis0i">disfali</ref>,
      <ref cel="implod.0i">implodi</ref>,
      <ref cel="krasx.0i">kra&scirc;i</ref>
    </refgrp>
  <trd lng="be">&c_k;&c_a;&c_l;&c_ja;&c_p;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>utrp&ecaron;t kolaps</trd>,
    <trd>zhroutit se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>kollabieren</trd>,
    <trd>zusammenbrechen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&ecirc;tre victime d'un <ind>collapsus</ind></trd>,
    <trd>s'<ind>abattre</ind></trd>,
    <trd>s'<ind>affaler</ind></trd>,
    <trd>s'<ind>affaisser</ind></trd>,
    <trd>s'<ind>&eacute;crouler</ind></trd>,
    <trd>s'<ind>effondrer</ind></trd>,
    <trd>craquer</trd>,
    <trd>crouler</trd>,
    <trd>d&eacute;gringoler</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x865A;&#33073;&#12377;&#x308B;  <pr>&#x304D;&#12423;&#12384;&#12387;&#12377;&#x308B;</pr></trd>,
    <trd>&#x5D29;&#x58CA;&#12377;&#x308B;  <pr>&#x307B;&#12358;&#x304B;&#12356;&#12377;&#x308B;</pr></trd>,
    <trd>&#x30D5;&#x30EA;&#x30FC;&#x30BA;&#12377;&#x308B;</trd>,
    <trd>&#x610F;&#x6C17;&#x6D88;&#x6C88;&#12377;&#x308B;  <pr>&#12356;&#x304D;&#12375;&#12423;&#12358;&#12385;&#12435;&#12377;&#x308B;</pr></trd>,
    <trd>&#34928;&#x5F31;&#12377;&#x308B;  <pr>&#12377;&#12356;&#12376;&#12419;&#x304F;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="pl">zawie&sacute;&cacute;</trd>
  <trdgrp lng="ro">
    <trd>e&#537;ua</trd>,
    <trd>tr&abreve;da</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>n&aacute;hle ochabn&uacute;&tcaron;</trd>,
    <trd>utrpie&tcaron; kolaps</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&c_je;&c_r;&c_je;&c_b;&c_u;&c_v;&c_a;&c_t;&c_i; &c_v; &#x43A;&#x43E;&#x43B;&c_a;&#x43F;&c_s;&c_ib;</trd>
  <trdgrp lng="zh">
    <trd>&#x5D29;&#x6E83; <pr>b&emacron;ngku&#xEC;</pr></trd>,
    <trd>&#x574D; <pr>t&amacron;n</pr></trd>,
    <trd>&#x584C; <pr>t&amacron;</pr></trd>,
    <trd>&#20498;&#x584C; <pr>d&#x1CE;ot&amacron;</pr></trd>,
    <trd>&#x574D;&#x584C; <pr>t&amacron;nt&amacron;</pr></trd>,
    <trd>&#x503E; <pr>q&#x12B;ng</pr></trd>,
    <trd>&#x54D7;&#21862; <pr>hu&#xE1;la</pr></trd>
  </trdgrp>
</drv>
</art>
