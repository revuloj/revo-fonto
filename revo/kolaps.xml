<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kolaps</rad>/o
</kap>
<drv mrk="kolaps.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Malsano karakterizata per sensinkopa, sed rapida malplii&gcirc;o
      de fortoj a&ubreve; funkcioj:
      <ekz>la <tld/>o estas ofta kaj grava simptomo de venenigoj;</ekz>
      <ekz>cerba, kora <tld/>o.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_ja;&c_p;&c_s;</trd>
  <trdgrp lng="de">
     <trd>Kollaps</trd>,
     <trd>Kreislaufversagen</trd>,
     <trd>Zusammenbruch</trd>
  </trdgrp>
  <trd lng="en">collapse</trd>
  <trdgrp lng="nl">
    <trd>instorting</trd>,
    <trd>inzinking</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_l;&c_a;&c_p;&c_s;</trd>
</drv>
</art>
