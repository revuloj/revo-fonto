<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>bombak</rad>/o
</kap>
<drv mrk="bombak.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="bombak.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Bombax</trd>), kun kotonecaj semoj.
    </dif>
  </snc>
  <trd lng="de">Wollbaum</trd>
</drv>
<drv mrk="bombak.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      tropikaj arboj (<trd lng="la">Bombacaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Wollbaumgew&auml;chse</trd>
</drv>
</art>
