<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>bombak</rad>/o
</kap>
<drv mrk="bombak.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="bombak1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Bombax</trd>), kun kotonecaj semoj.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_v;&c_o;&c_w;&c_n;&c_ib;&c_k;</trd>
  <trd lng="de">Wollbaum</trd>
  <trdgrp lng="hu">
    <trd>bombax</trd>,
    <trd>bolyfa</trd>,
    <trd>gyapotfa</trd></trdgrp>
  <trd lng="ru">&c_h;&c_l;&c_o;&c_p;&c_ch;&c_a;&c_t;&c_o;&c_je;
&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
</drv>
</art>
