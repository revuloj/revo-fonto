<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc><rad>afer</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="afer.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="afer.0o.cxio">
     <dif>
       &Ccirc;io ajn kio estas en la mondo, konkreta a&ubreve;
       abstrakta, nedifinite montrata:
       <ekz>
         sen povo kolero estas ridinda <tld/>o
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         dum mia voja&gcirc;o <ind>mi vidis interesajn <tld/>ojn</ind>;
         <trd lng="fi">olen n&auml;hnyt paljon mielenkiintoista</trd>
       </ekz>
       <ekz>
         <ind>gravaj <tld/>oj</ind>;
         <trd lng="pl">wa&zdot;ne sprawy</trd>
       </ekz>
       <ekz>
         scienca <tld/>o;
       </ekz>
       <ekz>
         <ind>pri kiu <tld/>o li parolas?</ind>;
         <trd lng="pl">o czym mowa?</trd>
       </ekz>
       <ekz>
         e&ccirc; la plej nekredeblajn <tld/>ojn li kredas<fnt>Z</fnt>.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="ajx.0o">a&jcirc;o</ref>,
       <ref cel="objekt.0o">objekto</ref>.
     </refgrp>
     <trd lng="br">tra</trd>
     <trdgrp lng="en">
       <trd>object</trd>,
       <trd>thing</trd>
     </trdgrp>
     <trdgrp lng="es">
       <trd>cosa</trd>,
       <trd>asunto</trd>
     </trdgrp>
     <trdgrp lng="fi">
       <trd>asia</trd>,
       <trd><klr>(voi tarkoittaa jotain konkreettistakin, j&auml;&auml;
         usein k&auml;&auml;nt&auml;m&auml;tt&auml;
         erillisell&auml; sanalla)</klr></trd>
     </trdgrp>
     <trdgrp lng="fr">
       <trd>chose</trd>,
       <trd>affaire<klr tip="ind"> (chose)</klr></trd>
     </trdgrp>
     <trd lng="hu">dolog</trd>
     <trdgrp lng="pl">
       <trd>sprawa</trd>,
       <trd>rzecz</trd>
     </trdgrp>
     <trdgrp lng="pt">
       <trd>coisa</trd>,
       <trd>assunto</trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>&c_v;&c_je;&c_shch;&c_mol;</trd>,
       <trd>&c_d;&c_je;&c_l;&c_o;</trd>
     </trdgrp>
     <trd lng="sk">vec</trd>
     <trdgrp lng="tr">
       <trd>&scedil;ey</trd>,
       <trd>e&scedil;ya</trd>
     </trdgrp>
   </snc>
   <snc mrk="afer.0o.objekto">
     <dif>
       Objekto de posedo, okupo, celo:
       <ekz>
         <ind>la <tld/>o ne ur&gcirc;as</ind>
         <fnt><bib>PrV</bib></fnt>;
         <trd lng="fr">il n'y a pas le <ind>feu</ind></trd>
       </ekz>
       <ekz>
         <ind>paroloj kaj faroj estas malsamaj <tld/>oj</ind>
         <fnt><bib>PrV</bib></fnt>;
         <trd lng="fr">entre dire et faire, il y a un <ind>monde</ind>
         </trd>
         <trd lng="pl">
         co innego m&oacute;wi&cacute;, a co innego robi&cacute;
         </trd>
         <trd lng="tr">lafla pilav pi&scedil;mez</trd>
       </ekz>
       <ekz>
         <ind>ne ju&gcirc;u pri <tld/>o la&ubreve; &gcirc;ia ekstero
         </ind>
         <fnt><bib>PrV</bib></fnt>;
         <trd lng="fi">ulkon&auml;k&ouml; pett&auml;&auml;</trd>
         <trdgrp lng="fr">
           <trd>il ne faut pas juger de l'arbre par
             l'<ind>&eacute;corce</ind></trd>,
           <trd>les <ind>apparence</ind>s sont trompeuses</trd>
         </trdgrp>
         <trd lng="pl">nie oceniaj rzeczy po wygl&aogonek;dzie</trd>
         <trd lng="tr">g&ouml;r&uuml;n&uuml;&scedil;e aldanmamal&inodot;</trd>
       </ekz>
       <ekz>
         aran&gcirc;i la <tld/>on
         <fnt><bib>MT</bib>, <lok>&Dan; 11:6</lok></fnt>;
       </ekz>
       <ekz>
         <ind>nun ni transiru al la <tld/>o</ind><fnt>Z</fnt>
         <klr>(la diskutata temo)</klr>;
         <trd lng="fr">venons-en &agrave; notre <ind>affaire</ind>
         </trd>
         <trd lng="pl">przejd&zacute;my do rzeczy</trd>
       </ekz>
       <ekz>
         <ind>al la afero!</ind>
         <fnt><bib>PrV</bib></fnt>;
         <trd lng="fi">asiaan!</trd>
         <trd lng="fr"><klr tip="ind">(venez-en) </klr>au <ind>fait
         </ind>!</trd>
         <trd lng="pl">do rzeczy!</trd>
       </ekz>
       <ekz>
         <ind>pri kio estas la <tld/>o?</ind>
         <fnt><bib>PrV</bib></fnt>;
         <trd lng="fr">de quoi s'<ind>agit</ind>-il?</trd>
         <trd lng="pl">o co chodzi?</trd>
       </ekz>
       <ekz>
         sa&gcirc;a tenas <tld/>on, malsa&gcirc;a esperon
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         mi formetas de mi &ccirc;ian
         oficialan rolon en nia afero<fnt>Z</fnt>;
       </ekz>
       <ekz>
         <ind>zorgu pri viaj <tld/>oj!</ind>;
         <trdgrp lng="fr">
           <trd>occupez-vous de vos <ind>affaire</ind>s!</trd>,
           <trd>occupez-vous de vos <ind>pied</ind>s!</trd>
         </trdgrp>
        <trd lng="pl">zajmij si&eogonek; zwoimi sprawami</trd>
        <trd lng="tr">kendi i&scedil;inizle u&gbreve;ra&scedil;&inodot;n</trd>
       </ekz>
       <ekz>
         <ind>tio estas alia <tld/>o</ind>;
         <trdgrp lng="fr">
           <trd>c'est une autre <ind>affaire</ind></trd>,
           <trd>c'est autre <ind>chose</ind></trd>
           <trd>c'est une autre paire de <ind>manche</ind>s</trd>
         </trdgrp>
         <trd lng="pl">to jest inna sprawa</trd>
       </ekz>
       <ekz>
         <ind>estas lia <tld/>o</ind> <klr>(tio koncernas lin)</klr>
         skribi al lia patro;
         <trdgrp lng="fr">
           <trd>c'est son <ind>affaire</ind></trd>,
           <trd>c'est son <ind>probl&egrave;me</ind></trd>,
           <trd>c'est lui que &ccedil;a <ind>regarde</ind></trd>
         </trdgrp>
        <trd lng="pl">to jest jego sprawa</trd>
        <trdgrp lng="tr">
        <trd>o kendisini ilgilendirir</trd>,
        <trd>o onun problemi</trd>
        </trdgrp>
       </ekz>
       <ekz>
         <ind>min ne tu&scirc;as la afero</ind>, mi staras ekstere
         <fnt><bib>PrV</bib></fnt>.
         <trdgrp lng="fr">
           <trd>ce n'est pas mon <ind>affaire</ind></trd>,
           <trd>ce n'est pas mon <ind>probl&egrave;me</ind></trd>,
           <trd>ce ne sont pas mes <ind>oignon</ind>s</trd>,
           <trd>cette affaire ne me <ind>regarde</ind> pas</trd>
         </trdgrp>
         <trd lng="pl">mnie ta sprawa nie dotyczy</trd>
         <trdgrp lng="tr">
           <trd>beni ilgilendirmez</trd>,
           <trd>o benim problemim de&gbreve;il</trd>
         </trdgrp>
       </ekz>
     </dif>
     <trd lng="br">afer</trd>
     <trdgrp lng="fi">
       <trd>asia</trd>,
       <trd>kysymys</trd>
     </trdgrp>
     <trdgrp lng="en">
       <trd>object</trd>,
       <trd>thing</trd>
     </trdgrp>
     <trdgrp lng="fr">
       <trd>affaire<klr tip="ind"> (qui occupe)</klr></trd>,
       <trd>probl&egrave;me<klr tip="ind"> (qui occupe)</klr></trd>,
       <trd>question<klr tip="ind"> (qui occupe)</klr></trd>,
     </trdgrp>
     <trd lng="hu">&uuml;gy</trd>
     <trdgrp lng="pl">
       <trd>sprawa</trd>,
       <trd>rzecz</trd>
     </trdgrp>
     <trdgrp lng="pt">
       <trd>quest&atilde;o</trd>,
       <trd>assunto</trd>,
       <trd>neg&oacute;cio</trd>
     </trdgrp>
     <trd lng="ru">&c_d;&c_je;&c_l;&c_o;</trd>
     <trd lng="sk">z&aacute;le&zcaron;itos&tcaron;</trd>
     <trdgrp lng="tr">
       <trd>problem</trd>,
       <trd>sorun</trd>,
         <trd>i&scedil;</trd>,
         <trd>dert</trd>
     </trdgrp>
   </snc>
   <snc mrk="afer.0o.SOC">
     <dif>
       Socia interago:
     </dif>
     <subsnc mrk="afer.0o.JUR">
       <uzo tip="fak">JUR</uzo>
       <dif>
         Tio, kio estas diskutata anta&ubreve; juristo:
         <ekz>
           mi esperas, ke mia <tld/>o balda&ubreve; venos anta&ubreve;
           la tribunalo;
         </ekz>
         <ekz>
           krima, sensacia <tld/>o.
         </ekz>
       </dif>
       <trd lng="br">afer</trd>
       <trdgrp lng="en">
         <trd>case</trd>
       </trdgrp>
       <trdgrp lng="fi">
         <trd>(oikeus)asia</trd>,
         <trd>tapaus</trd>,
         <trd>juttu</trd>
       </trdgrp>
       <trdgrp lng="es">
         <trd>caso</trd>,
         <trd>asunto</trd>
       </trdgrp>
       <trdgrp lng="fr">
         <trd>affaire<klr tip="ind"> (en justice)</klr></trd>,
         <trd>cas<klr tip="ind"> (en justice)</klr></trd>
         <trd>cause<klr tip="ind"> (en justice)</klr></trd>
         <trd>dossier<klr tip="ind"> (en justice)</klr></trd>
         <trd>proc&egrave;s</trd>
       </trdgrp>
       <trd lng="hu">&uuml;gy</trd>
       <trdgrp lng="pl">
         <trd>sprawa</trd>
       </trdgrp>
       <trdgrp lng="pt">
         <trd>caso</trd>,
         <trd>quest&atilde;o</trd>,
         <trd>causa</trd>
       </trdgrp>
       <trd lng="ru">&c_d;&c_je;&c_l;&c_o;</trd>
       <trd lng="sk">pr&iacute;pad</trd>
       <trdgrp lng="tr">
         <trd>dosya<klr>(hukuk)</klr></trd>,
         <trd>dava</trd>
       </trdgrp>
     </subsnc>
     <subsnc mrk="afer.0o.komerco">
       <uzo tip="fak">EKON</uzo>
       <dif>
         Komerco, negoco:
         <ekz>
           negocaj <tld/>oj estas severaj
           <fnt><bib>PrV</bib></fnt>;
         </ekz>
         <ekz>
           <ind>amikeco aparte, <tld/>o aparte</ind>
           <fnt><bib>PrV</bib></fnt>;
           <trdgrp lng="fr">
             <trd>en affaires, on ne fait pas de <ind>sentiment</ind>
             </trd>,
             <trd>les <ind>affaire</ind>s sont les affaires</trd>
           </trdgrp>
         </ekz>
         <ekz>
           tago festa, for <tld/>oj
           <fnt><bib>PrV</bib></fnt>.
         </ekz>
       </dif>
       <trd lng="br">afer</trd>
       <trd lng="en">concern</trd>
       <trd lng="fi">liikeasia</trd>
       <trdgrp lng="fr">
         <trd>affaire<klr tip="ind"> (commerciale)</klr></trd>,
         <trd>business<klr tip="ind"> (entreprise commerciale)</klr>
         </trd>
         <trd>entreprise</trd>,
         <trd>commerce</trd>,
         <trd>firme</trd>,
         <trd>soci&eacute;t&eacute;<klr tip="ind">
         (entreprise commerciale)</klr></trd>
       </trdgrp>
       <trdgrp lng="pl">
         <trd>sprawa</trd>,
         <trd>interes</trd>
       </trdgrp>
       <trdgrp lng="pt">
         <trd>assunto</trd>,
         <trd>neg&oacute;cio</trd>
       </trdgrp>
       <trd lng="ru">&c_d;&c_je;&c_l;&c_o;</trd>
       <trdgrp lng="tr">
         <trd>i&scedil;</trd>,
         <trd>&scedil;irket</trd>,
         <trd>firma</trd>,
         <trd>ticaret</trd>
         </trdgrp>            
     </subsnc>
   </snc>
   <trd lng="be">&c_s;&c_p;&c_r;&c_a;&c_v;&c_a;</trd>
   <trdgrp lng="ca">
     <trd>afer</trd>,
     <trd>assumpte</trd>
     <trd>cosa</trd>
   </trdgrp>
   <trdgrp lng="cs">
     <trd>v&ecaron;c</trd>,
     <trd>z&aacute;le&zcaron;itost</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>Sache</trd>,
     <trd>Angelegenheit</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>matter</trd>,
     <trd>affair</trd>
   </trdgrp>
   <trd lng="nl">zaak</trd>
   <trd lng="it">faccenda, affare, cosa</trd>
   <trdgrp lng="oc">
     <trd>afar</trd>,
     <trd>causa</trd>
   </trdgrp>
   <trd lng="tp">ijo</trd>
</drv>

<drv mrk="afer.0a">
   <kap><tld/>a</kap>
   <snc>
     <dif>
       Rilata al <tld/>o:
     </dif>
     <subsnc ref="afer.0o.objekto">
       <ekz>
         bestaj radikoj kaj multaj <tld/>aj radikoj signifas
         en verba formo <ctl>agi kiel tia besto a&ubreve; <tld/>o</ctl>
         <fnt>
         <aut>B. Wennergren</aut>
         <vrk>Verboj el ne-agaj radikoj</vrk>
         <lok>
<url ref=
"http://bertilow.com/pmeg/vortfarado/principoj/finajhoj/verbaj.html">
PMEG</url>
         </lok>
         </fnt>.
       </ekz>
       <trd lng="en">object-, related to objects</trd>
       <trd lng="fr">de <ind>chose</ind></trd>
     </subsnc>
     <subsnc ref="afer.0o.komerco">
       <ekz>
         &ccirc;e sociaj kaj <tld/>aj kunvenoj kutime kaj
         la&ubreve;more estas saluti kaj adia&ubreve;i &ccirc;iun
         personon individue, per manpremo
         <fnt>
         <bib>Voja&gcirc;o</bib>
         <aut>A. Tilmont</aut>
         <vrk>Praktikaj informoj pri Antverpeno</vrk><lok>leciono 11a
         </lok>
         </fnt>;
       </ekz>
       <ekz>
         retpo&scirc;to estas tre efika en persona kaj <tld/>a
         korespondado
         <fnt>
         <bib>Voja&gcirc;o</bib>
         <aut>B. Kolker</aut>
         <vrk>Pri Interreto per simplaj vortoj</vrk><lok>leciono 26a
         </lok>
         </fnt>.
       </ekz>
       <trd lng="en">business-, related to business</trd>
       <trd lng="fr">d'<ind>affaire</ind>(s)</trd>
     </subsnc>
     <trd lng="fi">asia-</trd>
   </snc>
</drv>

<drv mrk="afer.0eca">
   <kap><tld/>eca</kap>
   <snc>
     <dif>
       Havanta a&ubreve; afektanta seriozecon a&ubreve; alian
       econ, necesan por pritrakti gravajn <tld/>ojn:
       <ekz>
         afereca tono;
       </ekz>
       <ekz>
         fakte temas pri preska&ubreve; scienca faklibro
         en tre sobra <tld/>eca kaj preska&ubreve; senripro&ccirc;a
         lingva&jcirc;o
         <fnt>
         <aut>Pejno S.</aut>
         <vrk>Faligas la fundamentojn de esperantismo</vrk>
         <lok>
         <url ref="http://www.esperanto.be/fel/mon/rec/esmi.html">
         Monato</url>
         </lok>
         </fnt>.
       </ekz>
     </dif>
     <trd lng="en">business-like</trd>
     <trd lng="fi">asiallinen</trd>
   </snc>
</drv>

<drv mrk="afer.0ema">
   <kap><tld/>ema</kap>
   <snc>
     <dif>
       Inklina okupi&gcirc;i pri siaj <tld/>oj; tre okupita de ili,
       a&ubreve; &scirc;ajniganta sin tia:
       <ekz>
         anta&ubreve;e iras du <tld/>emaj homoj kaj petas kamaradojn
         gejunulojn iome doni vojon
         <fnt>
         <aut>A. Sol&jcirc;enicin trad L. Isajeva</aut>
         <vrk>La Paska Krucirado</vrk><lok>
         <url ref="http://esperanto.org/Ondo/Rn-solj1.htm">
         LOdE 1994:2</url></lok>
         </fnt>;
       </ekz>
       <ekz>
         tio &scirc;ajnas stranga, ke en <tld/>ema kaj agema Usono
         ekzistas tiuj personoj
         <fnt>
         <aut>trad V. Okc</aut>
         <vrk>Santa Feo</vrk>
         <lok>
         <url ref="http://www.esperanto.mv.ru/Usono/truu22.html">
         http://www.esperanto.mv.ru/Usono/truu22.html</url>
         </lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="en">business-like</trd>
   <trdgrp lng="fi">
     <trd>toimelias</trd>,
     <trd>puuhakas</trd>
   </trdgrp>
   <trd lng="fr">affair&eacute;</trd>
</drv>

<drv mrk="afer.0isto">
   <kap><tld/>isto</kap>
   <snc mrk="afer.0isto.EKON">
     <uzo tip="fak">EKON</uzo>
     <dif>
       Profesiulo pri <tld/>oj<sncref ref="afer.0o.komerco"/>:
       <ekz>
         lastatempe mi estis laborinta pli forte ol iu ajn el tiuj
         <tld/>istoj, kies sorton mi tiom arde volis eviti
         <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;ap. 10</lok></fnt>;
       </ekz>
       <ekz>
         Hadda estis ne nur teatrogeniulo, li estis anka&ubreve; ruza
         <tld/>isto, scianta kiom valoras bona organizado
         <fnt><bib>UrdHadda</bib>, <lok>p. 16</lok></fnt>;
       </ekz>
       <ekz>
         mi metis &ccirc;ion en <tld/>istan tekon
         <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;ap. 13</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="en">
     <trd>businessperson</trd>,
     <trd>businessman</trd>
   </trdgrp>
   <trdgrp lng="fi">
     <trd>liikemies</trd>,
     <trd>liikenainen</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>homme d'<ind>affaire</ind>s</trd>,
     <trd><ind>business</ind>-man</trd>
   </trdgrp>
   <trd lng="tr">i&scedil; adam&inodot;</trd>
</drv>

<drv mrk="afer.am0o">
   <kap>am<tld/>o</kap>
   <snc mrk="afer.am0o.KOMUNE">
     <dif>
       Ama kaj seksa interrilato.
     </dif>
   </snc>
   <trdgrp lng="en">
     <trd>affair</trd>,
     <trd>love affair</trd>
   </trdgrp>
   <trdgrp lng="fi">
     <trd>rakkausjuttu</trd>,
     <trd>suhde</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>aventure<klr tip="ind"> (amoureuse)</klr></trd>,
     <trd>liaison<klr tip="ind"> (amoureuse)</klr></trd>
   </trdgrp>
   <trd lng="tr">macera<klr>(a&scedil;k)</klr></trd>
</drv>

<drv mrk="afer.havi0onkun">
  <kap>havi <tld/>on kun</kap>
  <snc mrk="afer.havi0onkun.FRAZ">
    <uzo tip="stl">FRAZ</uzo>
    <dif>
      Pro negoco, ju&gcirc;afero a&ubreve; alia socia neceso
      rilati kun:
      <ekz>
        tiam la rabistoj komprenis, ke ili havas <tld/>on kun sekreta 
        sor&ccirc;a forto, kaj kun timego ili forkuris
        <fnt>
          <bib>Fab4</bib>,
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u ni povis scii, kun kiu kaj kio ni havos <tld/>on?
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/mondo.html">
          La mondo brilanta</url></vrk>,
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
