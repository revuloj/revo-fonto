<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>dent</rad>/o
</kap>
<drv mrk="dent.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="dent.0o.busxo">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Elstara osteto, en la bu&scirc;o de homoj kaj bestoj, kiu servas por mordi
      kaj ma&ccirc;i la nutra&jcirc;on:
      <ekz>
        <ind>muela <tld/>o</ind>;
        <trd lng="be">&c_m;&c_a;&c_l;&c_o;&c_ch;&c_n;&c_y; <ind>&c_z;&c_u;&c_b;</ind></trd>
        <trdgrp lng="nl">
          <trd>kies</trd>,
          <trd>baktand</trd>,
          <trd>maaltand</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind>kojna <tld/>o</ind>;
        <trd lng="nl">hoektand</trd>
      </ekz>
      <ekz>
        <ind>tran&ccirc;a <tld/>o</ind>;
        <trd lng="nl">snijtand</trd>
      </ekz>
      <ekz>
        la <tld/>oj klakas;
      </ekz>
      <ekz>
        kunfrapi la <tld/>ojn
        <fnt><bib>M</bib>, <lok>&Psa; 112:10</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        montri la <tld/>ojn
        <klr>(minaci)</klr>;
      </ekz>
      <ekz>
        nuksoj venis, kiam <tld/>oj elfalis
        <fnt><bib>PrV</bib></fnt>
        (<ref tip="vid" cel="fru.mal0a">malfrua</ref>)
      </ekz>
      <ekz>
        la <tld/>o de l' tempo formordis la piramidojn.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_u;&c_b;</trd>
    <trd lng="en">tooth</trd>
    <trdgrp lng="fr">
      <trd>dent</trd>,
      <trd>crochet<klr tip="ind"> (d'un serpent)</klr></trd>
    </trdgrp>
    <trd lng="nl">tand</trd>
    <trd lng="ru">&c_z;&c_u;&c_b;</trd>
  </snc>
  <snc mrk="dent.0o.FIG">
    <dif>
      Elstara pinto a&ubreve; tran&ccirc;a&jcirc;o &ccirc;e la rando a&ubreve;
      la supra&jcirc;o de objekto:
      <ekz>
        <tld/>o de kombilo, rado, segilo, muro, roko, monto, folio.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_u;&c_b;&c_je;&c_c;</trd>
    <trd lng="en">tooth</trd>
    <trdgrp lng="fr">
      <trd>dent<klr tip="ind"> (d'une scie)</klr></trd>,
      <trd>encoche</trd>
    </trdgrp>
    <trd lng="nl">tand</trd>
    <trd lng="ru">&c_z;&c_u;&c_b;&c_je;&c_c;</trd>
  </snc>
  <trd lng="cs">zub</trd>
  <trd lng="de">Zahn</trd>
  <trd lng="es">diente</trd>
  <trd lng="sk">zub</trd>
</drv>

<drv mrk="dent.0a">
  <kap><tld/>a</kap>
  <snc mrk="dent.0a.rilata_al_dento">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a konsonanto <klr>(t.e. <ref
        cel="dental.0o">dentalo</ref>)</klr>;
      </ekz>
      <ekz>
        <tld/>a emajlo.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_u;&c_b;&c_n;&c_y;</trd>
    <trd lng="en">dental</trd>
    <trd lng="es">dental</trd>
    <trdgrp lng="fr">
      <trd>dental<klr tip="ind"> (adj.)</klr></trd>,
      <trd>dentaire</trd>
    </trdgrp>
    <trd lng="nl">tand-</trd>
    <trd lng="ru">&c_z;&c_u;&c_b;&c_n;&c_o;&c_j;</trd>
  </snc>
  <snc mrk="dent.0a.denthava">
    <dif>
      <tld lit="D"/>hava:
      <ekz>
        <tld/>a rado.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_u;&c_b;&c_a;&c_s;&c_t;&c_y;</trd>, 
      <trd>&c_z;&c_u;&c_b;&c_a;&c_t;&c_y;</trd>, 
      <trd>&c_z;&c_u;&c_b;&c_ch;&c_a;&c_s;&c_t;&c_y;</trd>
      <trd>&c_z;&c_u;&c_b;&c_ch;&c_a;&c_t;&c_y;</trd>
    </trdgrp>
    <trd lng="es">dentado</trd>
    <trd lng="fr">dent&eacute;</trd>
    <trdgrp lng="ru">
      <trd>&c_z;&c_u;&c_b;&c_a;&c_s;&c_t;&c_y;&c_j;</trd>,
      <trd>&c_z;&c_u;&c_b;&c_ch;&c_a;&c_t;&c_y;&c_j;</trd>
    </trdgrp>
    <trd lng="nl">getand</trd>
  </snc>
  <trd lng="de">Zahn-</trd>
</drv>

<drv mrk="dent.0ego">
  <kap><tld/>ego</kap>
  <snc>
    <dif>
      Longa <tld/>o, kiu eliras el la bu&scirc;o de iuj bestoj (elefanto,
      apro).
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_k;&c_l;&c_a;</trd>, 
    <trd>&c_b;&c_ib;&c_v;&c_je;&c_n;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hauer</trd>, <trd>Sto&szlig;zahn</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>tusk</trd>,
    <trd>tush</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;fense<klr tip="ind"> (dent)</klr></trd>
  <trd lng="nl">slagtand</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_l;&c_y;&c_k;</trd>,
    <trd>&c_b;&c_i;&c_v;&c_je;&c_n;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">kel <klr>(slon&iacute;)</klr></trd>
</drv>

<drv mrk="dent.0aro">
  <kap><tld/>aro</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Tuto de la <tld/>oj:
      <ekz>
        li havas falsan <tld/>aron.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Gebiss</trd>
  <trd lng="es">dentadura</trd>
  <trdgrp lng="fr">
    <trd>denture</trd>,
    <trd>dentition<klr tip="ind"> (denture)</klr></trd>
  </trdgrp>
  <trd lng="nl">gebit</trd>
  <trd lng="sk">chrup</trd>
</drv>

<drv mrk="dent.0igi">
  <kap><tld/>(ig)i</kap>
  <snc>
    <dif>
      <tld lit="D"/>oforme tran&ccirc;i a&ubreve;
      aran&gcirc;i la randon de io:
      <ekz>
        <ind><tld/>ita</ind> rado, muro, falbalo, folio;
        <trd lng="be">&c_z;&c_a;&c_z;&c_u;&c_b;&c_r;&c_a;&c_n;&c_y;</trd>
        <trd lng="ru">&c_z;&c_a;&c_z;&c_u;&c_b;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
      </ekz>
      <ekz>
        la pintoj de la montaro segil<tld/>is la horizonton.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="brecx">bre&ccirc;eti</ref>,
      <ref cel="feston">festoni</ref>.
    </refgrp>
  </snc>
  <trd lng="fr">denteler</trd>
  <trd lng="nl">tanden</trd>
  <trd lng="ru">&c_z;&c_a;&c_z;&c_u;&c_b;&c_r;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="dent.0ingo">
  <kap><tld/>ingo</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Kava&jcirc;eto en la makzela osto, entenanta la radikon de <tld/>o.
    </dif>
  </snc>
  <trd lng="en">tooth socket</trd>
  <trd lng="de">Kieferh&ouml;hle</trd>
  <trd lng="fr">alv&eacute;ole dentaire</trd>
  <trd lng="nl">tandholte</trd>
</drv>

<drv mrk="dent.0okarno">
  <kap><tld/>okarno, <tld/>karno</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Karno de la makzeloj &ccirc;irka&ubreve;anta la <tld/>ajn radikojn.
    </dif>
    <refgrp tip="sin">
      <ref cel="gingiv.0o">gingivo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_ja;&c_s;&c_n;&c_a;</trd>
  <trd lng="de">Zahnfleisch</trd>
  <trd lng="en">gum</trd>
  <trd lng="es">encia</trd>
  <trd lng="fr">gencive</trd>
  <trd lng="nl">tandvlees</trd>
  <trd lng="ru">&c_d;&c_je;&c_s;&c_n;&c_a;</trd>
</drv>

<drv mrk="dent.0opikilo">
  <kap><tld/>opikilo, <tld/>pikilo</kap>
  <snc>
    <dif>
      Ligna spliteto por purigi inter la <tld/>oj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_u;&c_b;&c_a;&c_ch;&c_y;&c_s;&c_t;&c_k;&c_a;</trd>, 
    <trd>&c_k;&c_a;&c_l;&c_y;&c_p;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Zahnstocher</trd>
  <trd lng="en">toothpick</trd>
  <trd lng="ru">&c_z;&c_u;&c_b;&c_o;&c_ch;&c_i;&c_s;&c_t;&c_k;&c_a;</trd>
</drv>

<drv mrk="dent.0ostango">
  <kap><tld/>ostango</kap>
  <snc>
    <dif>
      Stango provizita per <tld/>oj, por kro&ccirc;i.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_u;&c_b;&c_ch;&c_a;&c_t;&c_a;&c_ja; &c_r;&c_e;&c_j;&c_k;&c_a;</trd>
  <trd lng="de">Zahnstange</trd>
  <trd lng="fr">cr&eacute;maill&egrave;re</trd>
  <trd lng="ru">&c_z;&c_u;&c_b;&c_ch;&c_a;&c_t;&c_a;&c_ja; &c_r;&c_je;&c_j;&c_k;&c_a;</trd>
</drv>

<drv mrk="dent.kojno0o">
  <kap>kojno<tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Unu el dentoj, situantaj inter tran&ccirc;aj kaj muelaj dentoj
      &ccirc;e homo a&ubreve; mamuloj.
    </dif>
    <ref tip="sin" cel="kanin.0o">kanino.</ref>
  </snc>
  <trd lng="be">&c_ib;&c_k;&c_l;&c_a;</trd>
  <trd lng="de">Eckzahn</trd>
  <trdgrp lng="en">
    <trd>eyetooth <klr>(p. p. dento de la supra makzelo)</klr></trd>,
    <trd>stomach tooth <klr>(p. p. dento de la malsupra
    makzelo)</klr></trd>
  </trdgrp>
  <trd lng="es">colmillo</trd>
  <trd lng="fr">canine</trd>
  <trd lng="nl">hoektand</trd>
  <trd lng="ru">&c_k;&c_l;&c_y;&c_k;</trd>
</drv>

<drv mrk="dent.tri0o">
  <kap>tri<tld/>o</kap>
  <snc>
    <dif>
      Forko kun tri <tld/>oj.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_y;&c_z;&c_u;&c_b;&c_je;&c_c;</trd>
  <trd lng="en">trident</trd>
  <trd lng="es">tridente</trd>
  <trd lng="fr">trident</trd>
  <trd lng="nl">drietand</trd>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_z;&c_u;&c_b;&c_je;&c_c;</trd>
  <trd lng="sk">trojzubec</trd>
</drv>

<drv mrk="dent.trancxo0o">
  <kap>tran&ccirc;o<tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <ref tip="dif" cel="incizi.0o">incizivo.</ref>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_je;&c_c; <klr tip="amb">(&c_z;&c_u;&c_b;)</klr></trd>
  <trd lng="en">incisor</trd>
  <trd lng="es">incisivo</trd>
  <trd lng="ru">&c_r;&c_je;&c_z;&c_je;&c_c; <klr tip="amb">(&c_z;&c_u;&c_b;)</klr></trd>
</drv>
</art>
