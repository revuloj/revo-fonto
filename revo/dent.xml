  <trdgrp lng="be">
    <trd>&c_ib;&c_k;&c_o;&c_l;</trd>,
    <trd>&c_b;&c_ib;&c_v;&c_je;&c_n;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&t_ma;&t_cha;&t_drengbu;&t_tsheg;&t_ba;&t_tsheg;</trd>
  <trd lng="ca">ullal</trd>
  <trd lng="cs">kel</trd>
