    <trd>&odblac;rl&odblac;fog</trd>
  </trdgrp>
  <trd lng="id"><ind>gigi</ind> geraham</trd>
  <trd lng="it">molare<klr> (sost.)</klr></trd>
  <trdgrp lng="nl">
    <trd>kies</trd>,
    <trd>baktand</trd>,
    <trd>maaltand</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>z&aogonek;b trzonowy</trd>,
    <trd>trzonowiec<klr> (z&aogonek;b)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_zh;&c_je;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;
  <ind>&c_z;&c_u;&c_b;</ind></trd>
  <trd lng="sk">zub stoli&ccaron;ka</trd>
</drv>

<drv mrk="dent.sagxo0o">
  <kap>sa&gcirc;o<tld/>o</kap>
  <snc mrk="dent.sagxo0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      <tld lit="D"/>o kiu elkreskas post
      <ref tip="ant" cel="dent.lakto0o.ANA">lakto<tld/>o</ref>,
