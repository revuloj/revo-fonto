    <trd>&c_z;&c_u;&c_b;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="zh">&#29273;&#31185;&#30340; <pr>y&aacute;k&emacron;de</pr></trd>
</drv>

<drv mrk="dent.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="dent.0aro.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Tuto de la <ref tip="prt" cel="dent.0o.busxo"><tld/>oj</ref>:
      <ekz>
        li havas falsan <tld/>aron;
      </ekz>
      <ekz>
        tordetante la supran lipon, <klr>[&#8230;]</klr> vidigante blankan, perlosimilan
        <tld/>aron
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        kial tiu spirito ne eniris en tigron, kiu hakas sian furiozan <tld/>aron en
        homan karnon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>?
      </ekz>
      <ekz>
        sub tiu kotona peco sin trovas longa vico de eburaj objektoj, aspektantaj kiel
        giganta <tld/>aro
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sidi&gcirc;inte kontra&ubreve; la minaca nigrablanka <tld/>aro, mi, bon&scirc;ance, neniam alvenis
        al la punkto, kiam la publiko rimarkis, ke mi tute ne scipovas ludi tiun
        instrumenton
        <fnt>
          <bib>Kon11</bib>
          <lok>Blogo de trompisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        gardu vin du baroj: lipoj kaj <tld/>aroj
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
