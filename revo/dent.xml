    <trd>&#27503;  <pr>&#12375;</pr></trd>,
    <trd>&#27503;&#29366;&#12398;&#29289;  <pr>&#12399;&#12376;&#12423;&#12358;&#12398;&#12406;&#12388;</pr></trd>,
    <trd>&#31361;&#36215;  <pr>&#12392;&#12387;&#12365;</pr></trd>
  </trdgrp>
  <trd lng="pl">z&aogonek;b</trd>
