</drv>

<drv mrk="dent.0a">
  <kap><tld/>a</kap>
  <snc mrk="dent.0a.rilata_al_dento">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <uzo tip="fak">FON</uzo>
        <ref cel="dent.0akonsonanto"><tld/>a konsonanto</ref>;
      </ekz><ekz>
        <uzo tip="fak">ANA</uzo>
        <tld/>a emajlo;
      </ekz>
      <ekz>
        objektoj eligitaj el homaj korpoj inkluzivas fajliletojn por purigi <tld/>ojn,
        protezojn kaj <tld/>ajn kronojn
        <fnt>
          <bib>Monato</bib>
          <aut>last/pg</aut>
          <vrk><url ref="&Monato;/2006/008852.php"
              >Fremdaj korpoj</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estus povinta havi iun ajn, <klr>[sed]</klr> li prenas knabineton kun grasa
        viza&gcirc;o kaj <tld/>a problemo
        <fnt>
          <bib>MJA</bib>
          <lok>Parto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        furoras &ccirc;e privataj klinikoj <tld/>aj <klr>[&#8230;]</klr> operacioj
        <fnt>
          <bib>Monato</bib>
          <aut>last/pg</aut>
          <vrk><url ref="&Monato;/2006/009000.php"
              >Medicina turismo</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_u;&c_b;&c_n;&c_y;</trd>
    <trd lng="ca">dental</trd>
    <trd lng="en">dental</trd>
    <trd lng="es">dental</trd>
    <trdgrp lng="fr">
      <trd>dental<klr tip="ind"> (adj.)</klr></trd>,
      <trd>dentaire</trd>
    </trdgrp>
    <trd lng="he">&dalet;&nun;&tet;&lamed;&yod;</trd>
    <trd lng="hu">fog-</trd>
    <trd lng="id">dental</trd>
    <trd lng="it">dentale</trd>
    <trd lng="nl">tand-</trd>
    <trd lng="pl">z&eogonek;bny</trd>
    <trd lng="ro">dintat</trd>
    <trd lng="ru">&c_z;&c_u;&c_b;&c_n;&c_o;&c_j;</trd>
  </snc>
  <snc mrk="dent.0a.denthava">
    <dif>
      <tld lit="D"/>hava:
      <ekz>
        <ind><tld/>a rado</ind>;
        <trd lng="he">&#8503;&lamed;&#8503;&lamed;
        &shin;&yod;&nun;&yod;&yod;&fmem;</trd>
      </ekz>
      <ekz>
        <klr>[imagu]</klr> lar&gcirc;an voston, platigitan de la flankoj, <tld/>an supre, kiel
        koka kresto
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> sinjorinoj preferas folipuntojn kun randoj <tld/>aj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; la tagi&gcirc;o <nom>Bhagat</nom> suriris la <tld/>an pinton por vidi, kiel sur la ne&gcirc;aj
        suproj eklumi&gcirc;as la matenru&gcirc;o
        <fnt>
          <bib>IKr</bib>
          <lok>La Miraklo De Purun Bhagat</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_u;&c_b;&c_a;&c_s;&c_t;&c_y;</trd>,
      <trd>&c_z;&c_u;&c_b;&c_a;&c_t;&c_y;</trd>,
      <trd>&c_z;&c_u;&c_b;&c_ch;&c_a;&c_s;&c_t;&c_y;</trd>,
      <trd>&c_z;&c_u;&c_b;&c_ch;&c_a;&c_t;&c_y;</trd>
    </trdgrp>
    <trd lng="ca">dentat</trd>
    <trd lng="es">dentado</trd>
    <trd lng="fr">dent&eacute;</trd>
    <trd lng="he">&mem;&shin;&vav;&nun;&fnun;</trd>
    <trdgrp lng="hu">
      <trd>fogas</trd>,
      <trd>fogazott</trd>
    </trdgrp>
    <trd lng="id"><baz>gigi</baz>bergigi</trd>
    <trd lng="it">dentato</trd>
    <trd lng="pl">z&eogonek;baty</trd>
    <trd lng="ro">dintat</trd>
    <trdgrp lng="ru">
      <trd>&c_z;&c_u;&c_b;&c_a;&c_s;&c_t;&c_y;&c_j;</trd>,
      <trd>&c_z;&c_u;&c_b;&c_ch;&c_a;&c_t;&c_y;&c_j;</trd>
    </trdgrp>
    <trd lng="nl">getand</trd>
  </snc>
  <trd lng="cs">zubat&yacute;</trd>
  <trd lng="de">Zahn-</trd>
  <trdgrp lng="ja">
    <trd>&#27503;&#12398;  <pr>&#12399;&#12398;</pr></trd>,
    <trd>&#27503;&#12398;&#12388;&#12356;&#12383;  <pr>&#12399;&#12398;&#12388;&#12356;&#12383;</pr></trd>
  </trdgrp>
  <trd lng="sk">zubat&yacute;</trd>
