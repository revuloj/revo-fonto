<drv mrk="dent.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="dent.0o.busxo">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Elstara <ref tip="super" cel="ost.0o">osteto</ref>, en la <ref tip="malprt"
      cel="busx.0o.ANA">bu&scirc;o</ref> de homoj kaj
      bestoj,
      kiu servas por mordi kaj ma&ccirc;i la nutra&jcirc;on:
      <ekz>
        la <tld/>oj de leono estas akraj
        <fnt><bib>F</bib>, <lok>&FE; 7</lok></fnt>;
      </ekz>
      <ekz>
        tie estos la plorado kaj la grincado de <tld/>oj
        <fnt><bib>NT</bib>, <lok>&Mat; 8:12</lok></fnt>;
      </ekz>
      <ekz>
        kunfrapi la <tld/>ojn
        <fnt><bib>MT</bib>, <lok>&Psa; 112:10</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        montri la <tld/>ojn
        <klr>(minaci)</klr>;
      </ekz>
      <ekz>
        nuksoj venis, kiam <tld/>oj elfalis
        <fnt><bib>PrV</bib></fnt>
        (<ref tip="vid" cel="fru.mal0a">malfrua</ref>)
      </ekz>
      <ekz>
        la <tld/>o de l' tempo formordis la piramidojn.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_u;&c_b;</trd>
    <trd lng="en">tooth</trd>
    <trdgrp lng="fr">
      <trd>dent</trd>,
      <trd>crochet<klr tip="ind"> (d'un serpent)</klr></trd>
    </trdgrp>
    <trd lng="nl">tand</trd>
    <trd lng="pl">z&aogonek;b</trd>
    <trd lng="ro">dinte</trd>
    <trd lng="ru">&c_z;&c_u;&c_b;</trd>
  </snc>
  <snc mrk="dent.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Elstara pinto a&ubreve; tran&ccirc;a&jcirc;o &ccirc;e la rando
      a&ubreve; la supra&jcirc;o de objekto:
      <ekz>
        <tld/>o de kombilo, rado, segilo, muro, roko, monto, folio.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_u;&c_b;&c_je;&c_c;</trd>
    <trd lng="ca">sortint</trd>
    <trd lng="en">tooth</trd>
    <trdgrp lng="fr">
      <trd>dent<klr tip="ind"> (d'une scie)</klr></trd>,
      <trd>encoche</trd>
    </trdgrp>
    <trd lng="nl">tand</trd>
    <trd lng="ru">&c_z;&c_u;&c_b;&c_je;&c_c;</trd>
  </snc>
  <trd lng="bg">&c_z;&c_malmol;&c_b;</trd>
  <trd lng="bo">&t_sa;&t_naro;&t_tsheg;</trd>
  <trd lng="ca">dent</trd>
  <trd lng="cs">zub</trd>
  <trd lng="de">Zahn</trd>
  <trd lng="es">diente</trd>
  <trd lng="he">&shin;&fnun;</trd>
  <trd lng="hu">fog</trd>
  <trd lng="id">gigi</trd>
  <trd lng="it">dente</trd>
  <trdgrp lng="ja">
    <trd>&#x6B6F;  <pr>&#12375;</pr></trd>,
    <trd>&#x6B6F;&#x72B6;&#x306E;&#29289;  <pr>&#x306F;&#12376;&#12423;&#12358;&#x306E;&#12406;&#12388;</pr></trd>,
    <trd>&#x7A81;&#x8D77;  <pr>&#12392;&#12387;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="pl">z&aogonek;b</trd>
  <trd lng="pt">dente</trd>
  <trd lng="ro">dinte</trd>
  <trd lng="sk">zub</trd>
  <trd lng="sw">jino</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_u;&c_b;</trd>,
    <trd>&c_z;&c_u;&c_b;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_u;&c_b;&c_ch;&c_i;&c_k; (&c_k;&c_o;&c_l;&c_je;&c_s;&c_a;)</trd>,
    <trd>&c_p;&c_ib;&c_k;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_ib;&c_ch;&c_n;&c_a; &c_v;&c_je;&c_r;&c_sh;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#29273;&#x9F52; <pr>y&#xE1;ch&#x1D0;</pr></trd>,
    <trd>&#29273; <pr>y&#xE1;</pr></trd>
  </trdgrp>
</drv>

