  </snc>
  <trd lng="cs">zubat&yacute;</trd>
  <trd lng="de">Zahn-</trd>
  <trdgrp lng="ja">
    <trd>&#x6B6F;&#x306E;  <pr>&#x306F;&#x306E;</pr></trd>,
    <trd>&#x6B6F;&#x306E;&#12388;&#12356;&#x305F;  <pr>&#x306F;&#x306E;&#12388;&#12356;&#x305F;</pr></trd>
  </trdgrp>
  <trd lng="sk">zubat&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_u;&c_b;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_u;&c_b;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="zh">&#29273;&#x79D1;&#30340; <pr>y&#xE1;k&emacron;de</pr></trd>
