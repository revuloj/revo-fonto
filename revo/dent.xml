<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>dent</rad>/o
</kap>
<drv mrk="dent.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="dent.0o.busxo">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Elstara <ref tip="super" cel="ost.0o">osteto</ref>, en la <ref
      tip="malprt" cel="busx.0o.ANA">bu&scirc;o</ref> de homoj kaj bestoj, kiu
      servas por mordi kaj ma&ccirc;i la nutra&jcirc;on:
      <ekz>
        la <tld/>oj de leono estas akraj
        <fnt><bib>F</bib>, <lok>&FE; 7</lok></fnt>;
      </ekz>
      <ekz>
        tie estos la plorado kaj la grincado de <tld/>oj
        <fnt><bib>NT</bib>, <lok>&Mat; 8:12</lok></fnt>;
      </ekz>
      <ekz>
        kunfrapi la <tld/>ojn
        <fnt><bib>MT</bib>, <lok>&Psa; 112:10</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        montri la <tld/>ojn
        <klr>(minaci)</klr>;
      </ekz>
      <ekz>
        nuksoj venis, kiam <tld/>oj elfalis
        <fnt><bib>PrV</bib></fnt>
        (<ref tip="vid" cel="fru.mal0a">malfrua</ref>)
      </ekz>
      <ekz>
        la <tld/>o de l' tempo formordis la piramidojn.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_u;&c_b;</trd>
    <trd lng="en">tooth</trd>
    <trdgrp lng="fr">
      <trd>dent</trd>,
      <trd>crochet<klr tip="ind"> (d'un serpent)</klr></trd>
    </trdgrp>
    <trd lng="nl">tand</trd>
    <trd lng="pl">z&aogonek;b</trd>
    <trd lng="ru">&c_z;&c_u;&c_b;</trd>
  </snc>
  <snc mrk="dent.0o.FIG">
    <dif>
      Elstara pinto a&ubreve; tran&ccirc;a&jcirc;o &ccirc;e la rando a&ubreve;
      la supra&jcirc;o de objekto:
      <ekz>
        <tld/>o de kombilo, rado, segilo, muro, roko, monto, folio.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_u;&c_b;&c_je;&c_c;</trd>
    <trd lng="en">tooth</trd>
    <trdgrp lng="fr">
      <trd>dent<klr tip="ind"> (d'une scie)</klr></trd>,
      <trd>encoche</trd>
    </trdgrp>
    <trd lng="nl">tand</trd>
    <trd lng="ru">&c_z;&c_u;&c_b;&c_je;&c_c;</trd>
  </snc>
  <trd lng="cs">zub</trd>
  <trd lng="de">Zahn</trd>
  <trd lng="es">diente</trd>
  <trd lng="he">&shin;&fnun;</trd>
  <trd lng="hu">fog</trd>
  <trd lng="pl">z&aogonek;b</trd>
  <trd lng="sk">zub</trd>
</drv>

<drv mrk="dent.0a">
  <kap><tld/>a</kap>
  <snc mrk="dent.0a.rilata_al_dento">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a konsonanto <klr>(t.e. <ref
        cel="dental.0o">dentalo</ref>)</klr>;
      </ekz>
      <ekz>
        <tld/>a emajlo.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_u;&c_b;&c_n;&c_y;</trd>
    <trd lng="en">dental</trd>
    <trd lng="es">dental</trd>
    <trdgrp lng="fr">
      <trd>dental<klr tip="ind"> (adj.)</klr></trd>,
      <trd>dentaire</trd>
    </trdgrp>
    <trd lng="he">&dalet;&nun;&tet;&lamed;&yod;</trd>
    <trd lng="hu">fog-</trd>
    <trd lng="nl">tand-</trd>
    <trd lng="pl">z&eogonek;bowy</trd>
    <trd lng="ru">&c_z;&c_u;&c_b;&c_n;&c_o;&c_j;</trd>
  </snc>
  <snc mrk="dent.0a.denthava">
    <dif>
      <tld lit="D"/>hava:
      <ekz>
        <ind><tld/>a rado</ind>.
        <trd lng="he">&gimel;&lamed;&gimel;&lamed; &shin;&yod;&nun;&yod;&yod;&fmem;</trd>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_u;&c_b;&c_a;&c_s;&c_t;&c_y;</trd>,
      <trd>&c_z;&c_u;&c_b;&c_a;&c_t;&c_y;</trd>,
      <trd>&c_z;&c_u;&c_b;&c_ch;&c_a;&c_s;&c_t;&c_y;</trd>,
      <trd>&c_z;&c_u;&c_b;&c_ch;&c_a;&c_t;&c_y;</trd>
    </trdgrp>
    <trd lng="es">dentado</trd>
    <trd lng="fr">dent&eacute;</trd>
    <trd lng="he">&mem;&shin;&vav;&nun;&fnun;</trd>
    <trdgrp lng="hu">
      <trd>fogas</trd>,
      <trd>fogazott</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_u;&c_b;&c_a;&c_s;&c_t;&c_y;&c_j;</trd>,
      <trd>&c_z;&c_u;&c_b;&c_ch;&c_a;&c_t;&c_y;&c_j;</trd>
    </trdgrp>
    <trd lng="nl">getand</trd>
  </snc>
  <trd lng="de">Zahn-</trd>
</drv>

<drv mrk="dent.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="dent.0aro.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Tuto de la <ref tip="prt" cel="dent.0o.busxo"><tld/>oj</ref>:
      <ekz>
        li havas falsan <tld/>aron.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Gebiss</trd>
  <trd lng="es">dentadura</trd>
  <trdgrp lng="fr">
    <trd>denture</trd>,
    <trd>dentition<klr tip="ind"> (denture)</klr></trd>
  </trdgrp>
  <trd lng="hu">fogazat</trd>
  <trd lng="nl">gebit</trd>
  <trd lng="pl">uz&eogonek;bienie</trd>
  <trd lng="sk">chrup</trd>
</drv>

<drv mrk="dent.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="dent.0ego.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Longa <ref tip="super" cel="dent.0o.busxo"><tld/>o</ref>, kiu eliras
      el la bu&scirc;o de tiaj bestoj, kiaj elefantoj, aproj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_k;&c_o;&c_l;</trd>,
    <trd>&c_b;&c_ib;&c_v;&c_je;&c_n;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hauer</trd>, <trd>Sto&szlig;zahn</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>tusk</trd>,
    <trd>tush</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;fense<klr tip="ind"> (dent)</klr></trd>
  <trd lng="he">&nun;&yod;&bet;</trd>
  <trd lng="hu">agyar</trd>
  <trd lng="nl">slagtand</trd>
  <trd lng="pl">kie&lstroke;</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_l;&c_y;&c_k;</trd>,
    <trd>&c_b;&c_i;&c_v;&c_je;&c_n;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">kel <klr>(slon&iacute;)</klr></trd>
</drv>

<drv mrk="dent.0igi">
  <kap><tld/>(ig)i</kap>
  <snc>
    <dif>
      <tld lit="D"/>oforme tran&ccirc;i a&ubreve; aran&gcirc;i la randon de io:
      <ekz>
        <ind><tld/>ita</ind> rado, muro, falbalo, folio;
        <trd lng="be">&c_z;&c_a;&c_z;&c_u;&c_b;&c_r;&c_a;&c_n;&c_y;</trd>
        <trd lng="he">&mem;&shin;&vav;&nun;&fnun;</trd>
        <trd lng="hu">fogazott ker&eacute;k</trd>
        <trd lng="ru">&c_z;&c_a;&c_z;&c_u;&c_b;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
      </ekz>
      <ekz>
        la pintoj de la montaro segil<tld/>is la horizonton.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="brecx">bre&ccirc;eti</ref>,
      <ref cel="feston">festoni</ref>
    </refgrp>
  </snc>
  <trd lng="fr">denteler</trd>
  <trd lng="hu">fogaz</trd>
  <trd lng="nl">tanden</trd>
  <trd lng="ru">&c_z;&c_a;&c_z;&c_u;&c_b;&c_r;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="dent.0ingo">
  <kap><tld/>ingo</kap>
  <snc mrk="dent.0ingo.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Kava&jcirc;eto en la makzela osto, entenanta la radikon de <tld/>o.
    </dif>
  </snc>
  <trd lng="en">tooth socket</trd>
  <trd lng="de">Kieferh&ouml;hle</trd>
  <trd lng="fr">alv&eacute;ole dentaire</trd>
  <trd lng="he">&mem;&kaf;&tav;&shin; &he;&shin;&fnun;</trd>
  <trd lng="hu">fog&uuml;reg</trd>
  <trd lng="nl">tandholte</trd>
  <trd lng="pl">dzi&aogonek;s&lstroke;o</trd>
</drv>

<drv mrk="dent.0opikilo">
  <kap><tld/>opikilo, <tld/>pikilo</kap>
  <snc>
    <dif>
      Ligna spliteto por purigi inter la <tld/>oj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_u;&c_b;&c_a;&c_ch;&c_y;&c_s;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_y;&c_p;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Zahnstocher</trd>
  <trd lng="en">toothpick</trd>
  <trd lng="fr">cure-dent</trd>
  <trd lng="he">&qof;&yod;&samekh;&fmem; &shin;&yod;&nun;&yod;&yod;&fmem;</trd>
  <trdgrp lng="hu">
    <trd>fogv&aacute;j&oacute;</trd>,
    <trd>fogpiszk&aacute;l&oacute;</trd>
  </trdgrp>
  <trd lng="pl">wyka&lstroke;aczka</trd>
  <trd lng="ru">&c_z;&c_u;&c_b;&c_o;&c_ch;&c_i;&c_s;&c_t;&c_k;&c_a;</trd>
</drv>

<drv mrk="dent.0ostango">
  <kap><tld/>ostango</kap>
  <snc>
    <dif>
      Stango provizita per <tld/>oj, por kro&ccirc;i.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_u;&c_b;&c_ch;&c_a;&c_t;&c_a;&c_ja; &c_r;&c_e;&c_j;&c_k;&c_a;</trd>
  <trd lng="de">Zahnstange</trd>
  <trd lng="fr">cr&eacute;maill&egrave;re</trd>
  <trd lng="he">&mem;&vav;&tet; &he;&yod;&lamed;&vav;&kaf;&yod;&fmem;</trd>
  <trdgrp lng="hu">
    <trd>fogasr&uacute;d</trd>,
    <trd>fogasl&eacute;c</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_u;&c_b;&c_ch;&c_a;&c_t;&c_a;&c_ja; &c_r;&c_je;&c_j;&c_k;&c_a;</trd>
</drv>

<drv mrk="dent.kojno0o">
  <kap>kojno<tld/>o, kojna <tld/>o</kap>
  <snc mrk="dent.kojno0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Iu el la mezaj, pintaj <ref tip="super"
      cel="dent.0o.busxo"><tld/>oj</ref> (inter tran&ccirc;aj kaj muelaj), kiuj
      &scirc;iras nutra&jcirc;on.
    </dif>
    <ref tip="sin" cel="kanin.0o">kanino</ref>
  </snc>
  <trd lng="be">&c_ib;&c_k;&c_o;&c_l;</trd>
  <trd lng="de">Eckzahn</trd>
  <trdgrp lng="en">
    <trd>eyetooth <klr>(p. p. dento de la supra makzelo)</klr></trd>,
    <trd>stomach tooth <klr>(p. p. dento de la malsupra
    makzelo)</klr></trd>
  </trdgrp>
  <trd lng="es">colmillo</trd>
  <trd lng="fr">canine<klr> (dent)</klr></trd>
  <trd lng="he">&nun;&yod;&bet;</trd>
  <trd lng="hu">szemfog</trd>
  <trd lng="nl">hoektand</trd>
  <trd lng="ru">&c_k;&c_l;&c_y;&c_k;</trd>
</drv>

<drv mrk="dent.muel0o">
  <kap>muel<tld/>o, muela <tld/>o</kap>
  <snc mrk="dent.muel0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Iu el la malanta&ubreve;aj, plataj <ref tip="super"
      cel="dent.0o.busxo"><tld/>oj</ref>, kiuj dispremas nutra&jcirc;on.
    </dif>
    <ref tip="sin" cel="molar.0o">molaro</ref>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_o;&c_ch;&c_n;&c_y; <ind>&c_z;&c_u;&c_b;</ind></trd>
  <trd lng="fr">molaire<klr> (dent)</klr></trd>
  <trd lng="he"><klr tip="ind">(&shin;&fnun;) </klr>&tet;&vav;&het;&nun;&tav;</trd>
  <trdgrp lng="hu">
    <trd>z&aacute;pfog</trd>,
    <trd>&odblac;rl&odblac;fog</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kies</trd>,
    <trd>baktand</trd>,
    <trd>maaltand</trd>
  </trdgrp>
</drv>

<drv mrk="dent.tri0o">
  <kap>tri<tld/>o</kap>
  <snc mrk="dent.tri0o.forko">
    <dif>
      <ref tip="super" cel="fork.0o">Forko</ref> kun tri <tld/>oj.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_y;&c_z;&c_u;&c_b;&c_je;&c_c;</trd>
  <trd lng="en">trident</trd>
  <trd lng="es">tridente</trd>
  <trd lng="fr">trident</trd>
  <trd lng="he">&qof;&lamed;&shin;&vav;&fnun;</trd>
  <trd lng="hu">h&aacute;rom&aacute;g&uacute; <ind>szigony</ind></trd>
  <trd lng="nl">drietand</trd>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_z;&c_u;&c_b;&c_je;&c_c;</trd>
  <trd lng="sk">trojzubec</trd>
</drv>

<drv mrk="dent.trancxo0o">
  <kap>tran&ccirc;o<tld/>o, tran&ccirc;a <tld/>o</kap>
  <snc mrk="dent.trancxo0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Iu el la anta&ubreve;aj <ref tip="super" cel="dent.0o.busxo"><tld/>oj</ref>,
      kiuj distran&ccirc;as nutra&jcirc;on.
    </dif>
    <ref tip="sin" cel="incizi.0o">incizivo</ref>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_je;&c_c; <klr tip="amb">(&c_z;&c_u;&c_b;)</klr></trd>
  <trd lng="de">Schneidezahn</trd>
  <trd lng="en">incisor</trd>
  <trd lng="es">incisivo</trd>
  <trd lng="fr">incisive<klr> (dent)</klr></trd>
  <trd lng="he">&shin;&fnun; &het;&vav;&tav;&kaf;&tav;</trd>
  <trd lng="hu">metsz&odblac;fog</trd>
  <trd lng="nl">snijtand</trd>
  <trd lng="ru">&c_r;&c_je;&c_z;&c_je;&c_c; <klr tip="amb">(&c_z;&c_u;&c_b;)</klr></trd>
</drv>
</art>
