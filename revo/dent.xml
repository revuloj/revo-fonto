<drv mrk="dent.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="dent.0ego.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda <ref tip="super" cel="dent.0o.busxo"><tld/>o</ref>; aparte tiu
