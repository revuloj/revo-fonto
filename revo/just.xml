<drv mrk="just.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      <ref tip="super" cel="konfor.0a.KOMUNE">Konforma</ref> kun la le&gcirc;o kaj juro:
      <ekz>
        ju&gcirc;u la popolon per ju&gcirc;o <tld/>a
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 16:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a decido
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 119:106</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rajt">rajta</ref>,
      <ref cel="legx.0a">le&gcirc;a</ref>.
      <ref cel="legiti">legitimi</ref>
    </refgrp>
    <trd lng="de">recht</trd>
    <trdgrp lng="hu">
      <trd>jogos</trd>,
      <trd>jogszer&udblac;</trd>,
      <trd>legitim</trd>
    </trdgrp>
  <trd lng="id">adil</trd>
  </snc>
  <snc>
    <dif>
      <ref cel="respek.0a">Respektanta</ref> egale kaj senpartie
      la rajtojn de aliaj homoj, kaj
      haviganta al &ccirc;iu lian apartena&jcirc;on:
      <ekz>
        tiu homo estis honesta, <tld/>a, diotima
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 1:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        nia provincestro estas severa, sed <tld/>a
        <fnt>
          <bib>F</bib>
          <lok>&FE; 37</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sola morale <tld/>a kaj praktike paciga intergenta principo devas esti plena
        sendependeco de &ccirc;iu lando de la religio kaj lingvo de &gcirc;iaj lo&gcirc;antoj
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Pri la homaranismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        ni devas esti <tld/>aj, ni devas taksi &ccirc;iun homon ne la&ubreve; lia venko a&ubreve; malvenko,
        sed la&ubreve; liaj laboroj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista en Boulogne
          sur Mer en la 5a de a&ubreve;gusto 1905</vrk>
        </fnt>;
      </ekz>
      <ekz>
        vi tiun &ccirc;i fojon denove forlasos mian domon <klr>[&#8230;]</klr> ne trovinta plenumon
        por viaj <tld/>aj <klr>(<ref cel="prav.0a">pravaj</ref>)</klr> postuloj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i estas prava, sin eltirinte el mia <tld/>a kolerego
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Deka</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="egal.0a.senprefera">egala<sncref/></ref>,
      <ref cel="ekvili.0o">ekvilibra</ref>,
      <ref cel="gxust.0a">&gcirc;usta</ref>,
      <ref cel="prav.0a">prava</ref>
    </refgrp>
    <trd lng="de">gerecht</trd>
    <trd lng="hu">igazs&aacute;gos</trd>
    <trd lng="id"><baz>tara</baz>setara</trd>
    <trd lng="sv">r&auml;ttvis</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_v;&c_ib;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_p;&c_r;&c_a;&c_v;&c_ja;&c_d;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>opr&aacute;vn&ecaron;n&yacute;</trd>,
    <trd>spravedliv&yacute;</trd>
  </trdgrp>
  <trd lng="en">just</trd>
  <trd lng="es">justo</trd>
  <trdgrp lng="fr">
    <trd>juste</trd>,
    <trd>l&eacute;gitime</trd>.
  </trdgrp>
  <trd lng="it">giusto</trd>
  <trdgrp lng="ja">
    <trd>&#x6B63;&#12375;&#12356;  <pr>&#x305F;&#12384;&#12375;&#12356;</pr></trd>,
    <trd>&#x6B63;&#x7FA9;&#x306E;  <pr>&#x305B;&#12356;&#x304E;&#x306E;</pr></trd>,
    <trd>&#x6B63;&#x5F53;&#x306A;  <pr>&#x305B;&#12356;&#12392;&#12358;&#x306A;</pr></trd>,
    <trd>&#21512;&#x6CD5;&#30340;&#x306A;  <pr>&#12372;&#12358;&#x307B;&#12358;&#12390;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x516C;&#x6B63;&#x306A;  <pr>&#12371;&#12358;&#x305B;&#12356;&#x306A;</pr></trd>,
    <trd>&#x516C;&#x5E73;&#x306A;  <pr>&#12371;&#12358;&#12408;&#12356;&#x306A;</pr></trd>
  </trdgrp>
  <trd lng="ne">&#2344;&#x93F;&#2359;&#x94D;&#x92A;&#2325;&#x94D;&#2359;</trd>
  <trd lng="nl">rechtvaardig</trd>
  <trd lng="pt">justo</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_r;&c_a;&c_v;&c_je;&c_d;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_v;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">spravodliv&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_p;&c_r;&c_a;&c_v;&c_je;&c_d;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_v;&c_i;&c_l;&c_mol;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_l;&c_u;&c_sh;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x516C;&#x5E73; <pr>g&#x14D;ngp&#xED;ng</pr></trd>,
    <trd>&#x79C9;&#x516C; <pr>b&#x1D0;ngg&#x14D;ng</pr></trd>,
    <trd>&#x6B63;&#x4E49; <pr>zh&#xE8;ngy&#xEC;</pr></trd>,
    <trd>&#x516C;&#20801; <pr>g&#x14D;ngy&#x1D4;n</pr></trd>,
    <trd>&#x516C;&#36947; <pr>g&#x14D;ngdao</pr></trd>
  </trdgrp>
</drv>

<drv mrk="just.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Karaktero de tio kio estas konforma je la <ref cel="jur.0o">juro</ref>:
      <ekz>
        &ccirc;io estas farata la&ubreve; rajto kaj <tld/>eco
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li decidis forkuri de la <tld/>eco
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        kanajlo <klr>[&#8230;]</klr>, kiu falsas la le&gcirc;ojn kaj
        kovras per ar&gcirc;ento la okulon de
        la <tld/>eco
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>eco proponas al vi liberecon, kaj vi ja efektive estas jam &gcirc;iaj kaptitoj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        pardonemeco superas <tld/>econ
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>jogoss&aacute;g</trd>,
      <trd>jogszer&udblac;s&eacute;g;</trd>,
      <trd>legitimit&aacute;s</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Morala virto, pri kiu oni respektas la rajtojn de proksimulo:
      <ekz>
        <tld/>eco, egaleco kaj frateco inter la popoloj en la praktiko estas plene ebla
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Dogmoj de Hilelismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        fidante al la dia <tld/>eco, &scirc;i atendis
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_r;&c_a;&c_v;&c_ja;&c_d;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_v;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&t_da;&t_rata;&t_nga;&t_tsheg;&t_ba;&t_da;&t_drengbu;&t_na;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>opr&aacute;vn&ecaron;nost</trd>,
    <trd>spravedlnost</trd>
  </trdgrp>
  <trd lng="de">Gerechtigkeit</trd>
  <trd lng="en">justice</trd>
  <trd lng="es">justicia</trd>
  <trd lng="fr">justice<klr tip="ind"> (caract&egrave;re juste)</klr></trd>
  <trd lng="hu">igazs&aacute;goss&aacute;g</trd>
  <trdgrp lng="id">
    <trd><baz>adil</baz>keadilan</trd>,
    <trd><baz>tara</baz>kesetaraan</trd>
  </trdgrp>
  <trd lng="it">giustizia</trd>
  <trdgrp lng="ja">
    <trd>&#x6B63;&#x7FA9;  <pr>&#x305B;&#12356;&#x304E;</pr></trd>,
    <trd>&#x6B63;&#x5F53;  <pr>&#x305B;&#12356;&#12392;&#12358;</pr></trd>,
    <trd>&#x516C;&#x6B63;  <pr>&#12371;&#12358;&#x305B;&#12356;</pr></trd>,
    <trd>&#x516C;&#x5E73;  <pr>&#12371;&#12358;&#12408;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">rechtvaardigheid</trd>
  <trd lng="pt">justi&ccedil;a</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_a;&c_v;&c_o;</trd>,
    <trd>&c_s;&c_p;&c_r;&c_a;&c_v;&c_je;&c_d;&c_l;&c_i;&c_v;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">spravodlivos&tcaron;</trd>
  <trd lng="sv">r&auml;ttvisa</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_a;&c_v;&c_o;</trd>,
    <trd>&c_s;&c_p;&c_r;&c_a;&c_v;&c_je;&c_d;&c_l;&c_i;&c_v;&c_ib;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4E49; <pr>y&#xEC;</pr></trd>,
    <trd>&#x6B63;&#x4E49; <pr>zh&#xE8;ngy&#xEC;</pr></trd>,
    <trd>&#x516C;&#36947; <pr>g&#x14D;ngd&#xE0;o</pr></trd>,
    <trd>&#x516C;&#x6B63; <pr>g&#x14D;ngzh&#xE8;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="just.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Ne <tld/>a:
      <ekz>
        mal<tld/>a kondamno
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dektria</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>aj kompromisoj
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Kion Zamenhof ne povis diri en &Gcirc;enevo.</vrk>
        </fnt>;
      </ekz>
      <ekz>
        vi estas mal<tld/>a kontra&ubreve; mi
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 16:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        se kontra&ubreve; iu stari&gcirc;os atestanto mal<tld/>a, akuzante lin pri krimo
        <klr>[&#8230;]</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 19:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>aj trezoroj ne donas utilon
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 10:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        sonis ankora&ubreve; en &scirc;iaj oreloj la mal<tld/>aj ripro&ccirc;oj
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ripro&ccirc;egis, li estis mal<tld/>a kaj senindulga
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_s;&c_p;&c_r;&c_a;&c_v;&c_ja;&c_d;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_p;&c_r;&c_a;&c_v;&c_ib;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bezpr&aacute;vn&yacute;</trd>,
    <trd>neopr&aacute;vn&ecaron;n&yacute;</trd>,
    <trd>nespravedliv&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>unrecht</trd>,
    <trd>ungerecht</trd>
  </trdgrp>
  <trd lng="en">unjust</trd>
  <trd lng="es">injusto</trd>
  <trdgrp lng="fr">
    <trd>injuste</trd>,
    <trd>ill&eacute;gitime</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>jogtalan</trd>,
    <trd>igazs&aacute;gtalan</trd>
  </trdgrp>
  <trd lng="it">ingiusto</trd>
  <trdgrp lng="ja">
    <trd>&#x4E0D;&#x6B63;&#x306A;  <pr>&#12405;&#x305B;&#12356;&#x306A;</pr></trd>,
    <trd>&#x4E0D;&#x516C;&#x5E73;&#x306A;  <pr>&#12405;&#12371;&#12358;&#12408;&#12356;&#x306A;</pr></trd>,
    <trd>&#x4E0D;&#x5F53;&#x306A;  <pr>&#12405;&#12392;&#12358;&#x306A;</pr></trd>,
    <trd>&#x4E0D;&#x6CD5;&#x306A;  <pr>&#12405;&#x307B;&#12358;&#x306A;</pr></trd>
  </trdgrp>
  <trd lng="nl">onrechtvaardig</trd>
  <trd lng="pt">injusto</trd>
  <trd lng="ru">&c_n;&c_je;&c_s;&c_p;&c_r;&c_a;&c_v;&c_je;&c_d;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>krivdiaci</trd>,
    <trd>nespravodliv&yacute;</trd>
  </trdgrp>
  <trd lng="sv">or&auml;ttvis</trd>
  <trd lng="uk">&c_n;&c_je;&c_s;&c_p;&c_r;&c_a;&c_v;&c_je;&c_d;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x51A4;&#26505; <pr>yu&amacron;nwang</pr></trd>,
    <trd>&#x4E0D;&#x4E49; <pr>b&#xF9;y&#xEC;</pr></trd>,
    <trd>&#x4E0D;&#x516C; <pr>b&#xF9;g&#x14D;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="just.mal0ajxo">
  <kap>mal<tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Mal<tld/>a ago:
      <ekz>
        suferi, toleri, ven&gcirc;i mal<tld/>a&jcirc;on;
      </ekz>
      <ekz>
        ne faru mal<tld/>a&jcirc;on en la ju&gcirc;ado, ne estu partia
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 19:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        kriantaj mal<tld/>a&jcirc;oj kaj perforta&jcirc;oj, kiuj &ccirc;iutage kaj sur &ccirc;iu pa&scirc;o estas
        farataj de la plimulto de lo&gcirc;antaro kontra&ubreve; la malplimulto
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Dogmoj de Hilelismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        kompensi al &scirc;i la mal<tld/>a&jcirc;on
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i a&ubreve;dis nenion krom moka mal<tld/>a&jcirc;o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        lia tuta animo bolis de indigno pro tiu mal<tld/>a&jcirc;o
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="difekt.0o">difekto</ref>,
      <ref cel="domagx.0o">doma&gcirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_s;&c_p;&c_r;&c_a;&c_v;&c_ja;&c_d;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="de">Unrecht</trd>
  <trd lng="en">injustice</trd>
  <trd lng="es">injusticia</trd>
  <trdgrp lng="fr">
    <trd>acte d'<ind>injustice</ind></trd>,
    <trd>injustice<klr tip="ind"> (acte d'injustice)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>jogtalan cselekedet</trd>,
    <trd>igazs&aacute;gtalan tett</trd>
  </trdgrp>
  <trd lng="id"><baz>adil</baz>ketidakadilan</trd>
  <trd lng="it">ingiustizia</trd>
  <trd lng="ja">&#x4E0D;&#x6B63;&#x884C;&#x70BA;  <pr>&#12405;&#x305B;&#12356;&#12371;&#12358;&#12356;</pr></trd>
  <trd lng="nl">onrecht</trd>
  <trd lng="pt">injusti&ccedil;a</trd>
  <trd lng="ru">&c_n;&c_je;&c_s;&c_p;&c_r;&c_a;&c_v;&c_je;&c_d;&c_l;&c_i;&c_v;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sv">or&auml;ttvisa</trd>
  <trdgrp lng="uk">
    <trd>&c_n;&c_je;&c_s;&c_p;&c_r;&c_a;&c_v;&c_je;&c_d;&c_l;&c_i;&c_v;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_r;&c_i;&c_v;&c_d;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4E0D;&#x516C;&#x6B63; <pr>b&#xF9;g&#x14D;ngzh&#xE8;ng</pr></trd>,
    <trd>&#x4E0D;&#x516C;&#x5E73; <pr>b&#xF9;g&#x14D;ngp&#xED;ng</pr></trd>,
    <trd>&#x51A4;&#x5C48; <pr>yu&amacron;nq&#x16B;</pr></trd>,
    <trd>&#x51A4; <pr>yu&amacron;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="just.mal0eco">
  <kap>mal<tld/>eco</kap>
  <snc>
    <dif>
      Manko de <tld/>eco:
      <ekz>
        socia mal<tld/>eco
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        lingva mal<tld/>eco
        <fnt>
          <bib>Kon11</bib>
          <lok>Interparolo kun Fernando Maia Jr.</lok>
        </fnt>;
      </ekz>
      <ekz>
