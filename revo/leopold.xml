<vortaro>
<art mrk="$Id: leopold.xml,v 1.8 2015/02/26 19:11:42 revo Exp $">
<kap>
  <rad>Leopold</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="leopold.0o">
  <kap><tld/>o</kap>
  <snc mrk="leopold.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
	<tld/>o kun duonfermitaj okuloj rigardadas sardinojn en la vazoj
	<fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>&leftquot;afablaj viroj, kiuj man&gcirc;as
	fi&scirc;ojn&rightquot;, p. 107a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_L;&c_je;&c_a;&c_p;&c_o;&c_l;&c_mol;&c_d;</trd>
  <trd lng="de">Leopold</trd>
  <trd lng="fr">L&eacute;opold</trd>
  <trd lng="it">Leopoldo</trd>
  <trd lng="nl">Leopold</trd>
  <trd lng="pl">Leopold</trd>
  <trd lng="pt">Leopoldo</trd>
</drv>
</art>

