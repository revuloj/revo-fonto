<drv mrk="best.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kondutanta krude kaj stulte:
      <ekz>
        <tld/>aj inklinoj, <tld/>a krimo;
      </ekz>
      <ekz>
        la <tld/>a bu&ccirc;ado en <nom>Bielostok</nom>
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Dua Kongreso Esperantista en Gen&egrave;ve en la 28a de a&ubreve;gusto 1906</vrk>
        </fnt>;
      </ekz>
      <ekz>
        doni al ili la eblon libere konati&gcirc;i kaj komuniki&gcirc;i
        sur ne&ubreve;trala fundamento kaj nur tiam povos malaperi tiaj
        <ind><tld/>a&jcirc;o</ind>j, kiujn ni nun vidas en diversaj
        lokoj
        <fnt>(<aut>Zamenhof</aut>: <vrk>Parolo anta&ubreve; la Dua
           Kongreso Esperantista en &Gcirc;enevo 1906</vrk>)
        </fnt>.
        <trdgrp lng="fr">
          <trd>atrocit&eacute;<klr tip="ind"> (acte
          bestial)</klr></trd>,
          <trd>acte bestial</trd>
        </trdgrp>
      </ekz>
    </dif>
    <trd lng="de">bestialisch</trd>
    <trd lng="en">animalistic</trd>
    <trdgrp lng="fr">
      <trd>animal<klr tip="ind"> (adj.)</klr></trd>,
      <trd>bestial</trd>
    </trdgrp>
    <trdgrp lng="io">
      <trd>bestala</trd>,
      <trd>bestatra</trd>
    </trdgrp>
    <trd lng="nl">dierlijk</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_y;&c_v;&c_jo;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_mol;&c_v;&c_ja;&c_r;&c_y;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x52D5;&#29289;&#x306E;  <pr>&#12393;&#12358;&#12406;&#12388;&#x306E;</pr></trd>,
    <trd>&#29539;&#x306E;&#12424;&#12358;&#x306A;  <pr>&#12369;&#12384;&#12418;&#x306E;&#x306E;&#12424;&#12358;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_v;&c_ib;&c_r;&c_i;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_v;&c_ib;&c_r;&#x44F;&c_ch;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="best.0oj">
