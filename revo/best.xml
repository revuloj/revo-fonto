<drv mrk="best.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="best.0aro.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Tuto de la <tld/>oj de iu regiono, terspaco, epoko;
      <ref tip="sin" cel="fauxn1.0o.ZOO">fa&ubreve;no</ref>:
      <ekz>
        kiu timas <tld/>aron, ne iru arbaron
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        iu, vidante ke el la tuta <tld/>aro nenia speco da bestoj man&gcirc;as bakitan panon,
        venis al la logika konvinko, ke &gcirc;i ne estas sana kaj bongusta
        <fnt>
          <bib>LR</bib>
          <lok>4. Fonetiko</lok>
        </fnt>;
      </ekz>
      <ekz>
        nun la tuta <tld/>aro estas en la man&gcirc;ejo por sin nutri
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni povos malkovri la alpan regionon, &gcirc;ian
        kreska&jcirc;aron,
        &gcirc;ian <tld/>aron
        <fnt>
          <aut>ffea-liono</aut>,
          <vrk><url
          ref="http://ffea-liono.over-blog.com/article-precizoj-pri-nia-sta-o-3-104090105.html">
          Precizoj pri nia Sta&gcirc;o</url></vrk>,
          <lok>2012-04-26</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x52A8;&#29289;&#x7FA4; <pr>d&#xF2;ngw&#xF9;q&#xFA;n</pr></trd>
</snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_w;&c_n;&c_a;</trd>,
    <trd>&c_s;&c_mol;&c_v;&c_je;&c_t; &c_zh;&c_y;&c_v;&c_jo;&c_l;&c_a;&c_w;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_zh;&c_i;&c_v;&c_o;&c_t;&c_i;&c_n;&c_s;&c_k;&c_i; &c_s;&c_v;&c_ja;&c_t;</trd>,
    <trd>&c_f;&c_a;&c_u;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">fauna</trd>
  <trdgrp lng="cs">
    <trd>zv&iacute;&rcaron;ectvo</trd>,
    <trd>zv&ecaron;&rcaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
