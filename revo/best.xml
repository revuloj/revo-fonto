  </trdgrp>
</drv>

<drv mrk="best.hejm0o">
  <kap>hejm<tld/>o</kap>
  <snc mrk="best.hejm0o.KOMUNE">
    <dif>
      Malsova&gcirc;a <tld/>o, kiun oni tenas hejme, kutime por akompano a&ubreve;
      plezuro:
      <ekz>
        Tiuj novaj &rightquot;kreita&jcirc;oj&#x201D; povas tre rapide malfermi la pordon al
        kreado de aliaj modifitaj hejm<tld/>oj
        <fnt>
          <aut>Franck Mazoyer</aut>,
          <vrk><url ref="https://eo.mondediplo.com/article851.html">
            Konsekro de la mutaciulo</url></vrk>,
          <lok>Le Monde diplomatique en Esperanto, 2004-01-01</lok>
      </fnt>;
      </ekz>
      <ekz>
        Kiel hejm<tld/>oj la blanka varia&jcirc;o estas plej populara, pro tio
        ke &gcirc;i aspektas amuze, sed anka&ubreve; malhelaj aksolotloj estas
        vendataj
        <fnt>
          <aut>Frank van Hertrooij</aut>
          <vrk><url ref="https://scivolemo.wordpress.com/2018/10/03/la-salamandro-kiu-neniam-igas-plenkreska/">
          La salamandro kiu neniam i&gcirc;as plenkreska</url></vrk>,
          <lok>Scivolemo, 2018-10-03</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="best.dorlot0o">dorlot<tld/>o</ref>
  </snc>
  <trd lng="bg">&c_d;&c_o;&c_m;&c_a;&c_sh;&c_je;&c_n; &c_l;&c_ju;&c_b;&c_i;&c_m;&c_je;&c_c;</trd>
  <trd lng="ca">animal de companyia</trd>
  <trd lng="de">Haustier</trd>
  <trd lng="en">pet</trd>
  <trd lng="fr">animal de compagnie</trd>
  <trd lng="id"><ind>binatang</ind> peliharaan</trd>
  <trd lng="ja">&#x30DA;&#x30C3;&#x30C8; [<ind>&#x307A;&#x3063;&#x3068;</ind>]</trd>
  <trd lng="nl">huisdier</trd>
</drv>

<drv mrk="best.mam0oj">
  <kap>mam<tld/>oj</kap>
  <snc mrk="best.mam0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="mam.0uloj">Mamuloj</ref>:
      <ekz>
        la ovo kovi&gcirc;as en la organismo de la
        mam<tld/>o
        <fnt>
          <aut>G. R. Ledon</aut>,
          <vrk>Ovo kaj kokino</vrk>,
          <bib>Monato</bib>
          <lok>2001:11, p. 5a</lok>
        </fnt>;
      </ekz>
      <ekz>
        gravan rolon en disvastigo de rabio havas sangosu&ccirc;antaj vespertoj, kiuj sin
        nutras per sango de mam<tld/>oj kaj infektas ilin
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="https://www.monato.be/2008/009550.php">Batalo kontra&ubreve; mortiga
          malsano</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_b;&c_o;&c_z;&c_a;&c_j;&c_n;&c_i;&c_c;&c_i;</trd>,
    <trd>&c_m;&c_l;&c_je;&c_k;&c_o;&c_p;&c_i;&c_t;&c_a;&c_je;&c_shch;&c_i;</trd>
  </trdgrp>
  <trd lng="br">bronneged</trd>
  <trd lng="ca">mam&iacute;fers</trd>
  <trd lng="cs">savce</trd>
  <trd lng="de">S&auml;ugetiere</trd>
  <trd lng="en">mammals</trd>
  <trd lng="fr">mammif&egrave;res</trd>
  <trd lng="he">&yod;&vav;&nun;&qof;&yod;&fmem;</trd>
  <trd lng="hu">eml&odblac;s&aacute;llatok</trd>
  <trd lng="id">mamalia</trd>
  <trd lng="la">Mammalia</trd>
  <trd lng="nl">zoogdieren</trd>
  <trd lng="pl">ssaki</trd>
  <trd lng="ru">&c_m;&c_l;&c_je;&c_k;&c_o;&c_p;&c_i;&c_t;&c_a;&c_ju;&c_shch;&c_i;&c_je;</trd>
  <trd lng="sk">cicavce</trd>
  <trd lng="tr">memeliler</trd>
</drv>

<drv mrk="best.rabo0o">
  <kap>rabo<tld/>o</kap>
  <snc mrk="best.rabo0o.iu_el">
    <dif>
      Iu el <ref tip="malprt" cel="best.rabo0oj">rabo<tld/>oj</ref>:
      <ekz>
        ne estos tie leono, kaj rabo<tld/>o ne aperos
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 35:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        aliaj gardas, ke <klr>[&#x2026;]</klr> rabo<tld/>oj ilin ne dis&scirc;iru
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        defendi sin kontra&ubreve; rabo<tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Mihail Korotkov</aut>
          <vrk><url ref="https://www.monato.be/2011/010218.php">La origino de la homaj
          lingvoj</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_h;&c_i;&c_shch;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_m;&c_je;&c_s;&c_o;&c_ja;&c_d;&c_n;&c_o; &c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dravec</trd>,
    <trd>&scaron;elma</trd>
  </trdgrp>
  <trd lng="fr">carnivore</trd>
  <trdgrp lng="sk">
    <trd>dravec</trd>,
    <trd>&scaron;elma</trd>
  </trdgrp>
</drv>

