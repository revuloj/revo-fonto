<drv mrk="best.sxargxo0o">
  <kap>&scirc;ar&gcirc;o<tld/>o,
  <var><kap>&scirc;ar&gcirc;<tld/>o</kap></var></kap>
  <snc mrk="best.sxargxo0o.KOMUNE">
    <dif>
      <tld lit="B"/>o uzata por portado de peza&jcirc;oj:
      <ekz>
        azenido, filo de &scirc;ar&gcirc;o<tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 21:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi ne pensas, ke la &scirc;ar&gcirc;<tld/>o, kiu estas vipata, malsatas kaj morte laci&gcirc;as
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi vidis mian amikon kaj liajn du &scirc;ar&gcirc;<tld/>ojn
        <fnt>
          <aut>E. R. Burroughs, trad. K. R. C. Sturmer</aut>,
          <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0029/index.php">
          Princino de Marso</url></vrk>,
          <lok>1938</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_t;&c_o;&c_v;&c_a;&c_r;&c_n;&c_o; &c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_o;</trd>
  <trd lng="de">Lasttier</trd>
  <trdgrp lng="en">
    <trd>pack animal</trd>,
    <trd>beast of burden</trd>
  </trdgrp>
  <trd lng="fr">b&ecirc;te de somme</trd>
  <trd lng="hu">igavon&oacute; barom</trd>
  <trdgrp lng="io">
    <trd>charjobestio</trd>,
    <trd>kargobestio</trd>
  </trdgrp>
  <trd lng="ja">&#x99C4;&#x7363; [<ind>&#x3060;&#x3058;&#x3085;&#x3046;</ind>]</trd>
  <trdgrp lng="nl">
    <trd>lastdier</trd>,
    <trd>pakdier</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_mol;&c_ju;&c_ch;&c_n;&c_o;&c_je;
  &c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_o;&c_je;</trd>
</drv>

<drv mrk="best.tir0o">
  <kap>tir<tld/>o</kap>
  <snc mrk="best.tir0o.KOMUNE">
    <dif>
      <tld lit="B"/>o &ndash; ordinare granda kvarpiedulo &ndash;
      kiun oni uzas por tirado de veturilo a&ubreve; aparato:
      <ekz>
        la bovino ne donados lakton, &scirc;i <klr>[&#x2026;]</klr> povas ta&ubreve;gi nur kiel tir<tld/>o
        a&ubreve; en bu&ccirc;ejo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Feli&ccirc;a Jo&ccirc;jo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li donis al multaj kamparanoj atestilojn ke
        iliaj &ccirc;evaloj ne ta&ubreve;gas por militservo kiel
        tir<tld/>oj
        <fnt>
          <aut>T. Konopka</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/32594/218712">
          Familiaj Sortoj</url></vrk>,
          <lok>2010-01-07</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_v;&c_p;&c_r;&c_je;&c_g;&c_a;&c_t;&c_n;&c_o; &c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_o;</trd>
  <trd lng="ca">animal de tir</trd>
  <trd lng="cs">ta&zcaron;n&eacute; zv&iacute;&rcaron;e</trd>
  <trd lng="de">Zugtier</trd>
