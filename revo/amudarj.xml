<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Amudarj</rad>/o
    </kap>

    <drv mrk="amudarj.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
	<dif>
	  La plej granda rivero de la
	  <ref tip="malprt" cel="azi.InternaA0o">Interna Azio</ref>,
	  2540km longa, naski&gcirc;anta el kunfluo de Pjan&gcirc;o kaj Va&hcirc;&scirc;o,
	  kaj enfluanta en (la Grandan) <ref cel="aral.0o">Aralon</ref>.
	</dif>
      </snc>
      <trdgrp lng="en">
	<trd>Amu Darya</trd>,
	<trd>Oxus</trd>
      </trdgrp>
      <trd lng="hu">Amu-darja</trd>
      <trdgrp lng="ru">
	<trd>&c_A;&c_m;&c_u;</trd>,
	<trd>&c_A;&c_m;&c_u;&c_d;&c_a;&c_r;&c_mol;&c_ja;</trd>,
	<trd>&c_B;&c_a;&c_l;&c_h;</trd>,
	<trd>&c_O;&c_k;&c_s;</trd>
      </trdgrp>
    </drv>

  </art>

