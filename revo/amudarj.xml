<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: amudarj.xml,v 1.18 2023/08/14 11:19:57 revo Exp $">
  <kap>
    <rad>Amudarj</rad>/o <fnt><bib>BoER</bib></fnt>
  </kap>

<drv mrk="amudarj.A0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
    <dif>
      La plej granda <ref tip="lst" cel="river.0o" lst="voko:riveroj">rivero</ref> de la
      <ref tip="malprt" cel="azi.InternaA0o">Interna Azio</ref>,
      2540&nbsp;km longa, naski&gcirc;anta el kunfluo de
      <ref cel="pjangx.P0o">Pjan&gcirc;o</ref> kaj Va&hcirc;&scirc;o,
      kaj enfluanta en (la Grandan) <ref cel="aral.A0o">Aralon</ref>.
      <ekz>
        Turkmenio <klr>[&#8230;]</klr> estas regata de la Karakuma dezerto,
        en &gcirc;ia teritorio trairas la riveroj <tld/>o, <nom>Atrak</nom> kaj <nom>Murgab</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4190">Turkmenio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        mia naski&gcirc;loko (Uzbekio) aperas en la notoj jen kiel Transoksanio (persa
        vidpunkto, mian tutan vivon mi lo&gcirc;as sur tiu &leftquot;transa&rightquot; flanko de <tld/>o
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470;3</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_A;&c_m;&c_u;&c_d;&c_a;&c_r;'&c_ja;</trd>
  <trd lng="bg">&c_A;&c_m;&c_u;&c_d;&c_a;&c_r;&c_ja;</trd>
  <trd lng="cs">Amudarja</trd>
  <trd lng="de">Amudarja</trd>
  <trdgrp lng="en">
    <trd>Amu Darya</trd>,
    <trd>Oxus</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_m;&a_w;&a_d;&a_r;&f_y;&a_A;</trd>,
    <trd>&a_j;&f_y;&a_H;&a_w;&a_n;</trd>,
    <trd>&a_w;&a_kh;&a_sh;&a_w;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Amou-Daria</trd>,
    <trd>Oxus</trd>,
    <trd>Jihoun</trd>
  </trdgrp>
  <trd lng="hu">Amu-darja</trd>
  <trd lng="pl">Amu-daria</trd>
  <trdgrp lng="ru">
    <trd>&c_A;&c_m;&c_u;</trd>,
    <trd>&c_A;&c_m;&c_u;&c_d;&c_a;&c_r;&c_mol;&c_ja;</trd>,
    <trd>&c_B;&c_a;&c_l;&c_h;</trd>,
    <trd>&c_O;&c_k;&c_s;</trd>
  </trdgrp>
  <trd lng="sk">Amudarja</trd>
  <trd lng="tr">Amuderya</trd>
  <trd lng="uk">&c_A;&c_m;&c_u;&c_d;&c_a;&c_r;&#8217;&c_ja;</trd>
</drv>

</art>

