<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: afrank.xml,v 1.20 2022/03/11 12:50:04 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>afrank</rad>/i
</kap>
<drv mrk="afrank.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">POSX</uzo>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Anta&ubreve;pagi la sendkostojn de letero a&ubreve; paka&jcirc;o:
      <ekz>
        nesufi&ccirc;e <tld/>ita letero estigas monpunon por la
        adresato.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_l;&c_o;&c_ch;&c_v;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_p;&c_a;&c_sh;&c_t;&c_o;&c_v;&c_y;&c_ja;
  &c_a;&c_d;&c_p;&c_r;&c_a;&c_w;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_ib;)</klr></trd>
  <trdgrp lng="cs">
    <trd>frankovat</trd>,
    <trd>ozn&#xE1;mkovat</trd>,
    <trd>vyplatit <klr>(dopis)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>frankieren</trd>,
     <trd>freimachen</trd>
  </trdgrp>
  <trd lng="en">prepay postage</trd>
  <trd lng="es"><ind>franquear</ind> (correos)</trd>
  <trd lng="fr">affranchir</trd>
  <trd lng="it">affrancare</trd>
  <trd lng="nl">frankeren</trd>
  <trdgrp lng="pt">
    <trd>franquear</trd>,
    <trd><ind>selar</ind> (carta ou pacote)</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_f;&c_r;&c_a;&c_n;&c_k;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_p;&c_l;&c_a;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol; &c_p;&c_o;&c_ch;&c_t;&c_o;&c_v;&c_y;&c_m;&c_i; &c_m;&c_a;&c_r;&c_k;&c_a;&c_m;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ofrankova&#x165;</trd>,
    <trd>ozn&#xE1;mkova&#x165;</trd>
  </trdgrp>
  <trd lng="sv">frankera</trd>
  <trdgrp lng="tr">
        <trd>pulunu yap&inodot;&scedil;t&inodot;rmak</trd>,
        <trd> &ouml;nceden &ouml;demek<klr>(masraf&inodot;n&inodot;)</klr></trd>,
        <trd>&ouml;zg&uuml;r k&inodot;lmak</trd>,
        <trd>vergiyi kald&inodot;rmak</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x43F;&#x43B;&#x430;&#x447;&#x443;&#x432;&#x430;&#x442;&#x438; &#x43F;&#x43E;&#x448;&#x442;&#x43E;&#x432;&#x438;&#x43C;&#x438; &#x43C;&#x430;&#x440;&#x43A;&#x430;&#x43C;&#x438;</trd>,
    <trd>&#x444;&#x440;&#x430;&#x43D;&#x43A;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="afrank.0ite">
  <kap><tld/>ite</kap>
  <uzo tip="fak">POSX</uzo>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Anta&ubreve;pagite &ccirc;e po&scirc;to.
    </dif>
  </snc>
  <trd lng="cs">vyplacen&#x11B; <klr>(p&#x159;epravn&#xE9; uhrazeno)</klr></trd>
  <trdgrp lng="de">
     <trd>frankiert</trd>,
     <trd>freigemacht</trd>
  </trdgrp>
  <trd lng="en">postage prepaid</trd>
  <trd lng="fr">affranchi</trd>
  <trd lng="nl">port betaald</trd>
  <trdgrp lng="pt">
    <trd>franqueado</trd>,
    <trd>com <ind>porte pago</ind></trd>
  </trdgrp>
  <trd lng="sk">vyplatene <klr>(prepravn&#xE9; uhraden&#xE9;)</klr></trd>
</drv>
</art>
