<drv mrk="mensog.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc mrk="mensog.0i.KOMUNE">
    <gra><vspec>x</vspec></gra>
    <dif>
      Intence diri malvera&jcirc;on:
      <ekz>
        senhonte <tld/>i<fnt>Z</fnt>;
      </ekz><ekz>
        <tld/>i ma&scirc;ine<fnt>Z</fnt>;
      </ekz><ekz>
        <tld/>i kiel kalendaro<fnt>Z</fnt>;
      </ekz><ekz>
        <tld/>i kiel funebra parolo<fnt>Z</fnt>;
      </ekz><ekz>
        <ind><tld/>i kiel gazeto</ind>
        <trd lng="fr">mentir comme un arracheur de dents</trd>
        <fnt>Z</fnt>;
      </ekz><ekz>
        <tld/>a atestanto<fnt>Z</fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i indiferentecon
        <klr>(trompe &scirc;ajnigi)</klr>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        tiu &ccirc;ambro <tld/>is komforton.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxajn.0igi">&scirc;ajnigi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_l;&c_u;&c_s;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_n;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_ib;&c_l;&c_g;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&t_sa;&t_kata;&t_yata;&t_ga;&t_tsheg;&t_ra;&t_dzata;&t_shabchu;&t_na;&t_tsheg;&t_ba;&t_sha;&t_da;&t_tsheg;</trd>
  <trdgrp lng="br">
    <trd>gaouiadi&ntilde;</trd>,
    <trd>konta&ntilde; <ind>gevier</ind></trd>
  </trdgrp>
  <trd lng="ca">mentir</trd>
  <trd lng="cs">lh&aacute;t</trd>
  <trd lng="de">l&uuml;gen</trd>
  <trd lng="en">lie</trd>
  <trd lng="es">mentir</trd>
  <trd lng="fr">mentir</trd>
  <trd lng="hu">hazudik</trd>
  <trd lng="id">ber<ind>bohong</ind></trd>
  <trd lng="it">mentire</trd>
  <trd lng="nl">liegen</trd>
  <trd lng="oc">mentir</trd>
  <trd lng="pl">k&lstroke;ama&cacute;</trd>
  <trd lng="pt">mentir</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_g;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_r;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>cig&aacute;ni&tcaron;</trd>,
    <trd>luha&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_r;&c_je;&c_h;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_g;&c_o;&c_v;&c_o;&c_r;&c_i;&c_t;&c_i; &c_n;&c_je;&c_p;&c_r;&c_a;&c_v;&c_d;&c_u;</trd>
  </trdgrp>
</drv>

