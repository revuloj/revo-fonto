<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mensog</rad>/i
</kap>
<drv mrk="mensog.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc>
    <gra><vspec>x</vspec></gra>
    <dif>
      Intence diri malvera&jcirc;on:
      <ekz>
        senhonte <tld/>i<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i ma&scirc;ine<fnt>Z</fnt>,
      </ekz>
      <ekz>
        kiel kalendaro<fnt>Z</fnt>,
      </ekz>
      <ekz>
        kiel funebra parolo<fnt>Z</fnt>,
      </ekz>
      <ekz>
        kiel gazeto<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a atestanto<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>ema bu&scirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i <klr>(trompe &scirc;ajnigi)</klr> indiferentecon;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu &ccirc;ambro <tld/>is komforton.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_l;&c_u;&c_s;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_n;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_ib;&c_l;&c_g;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">l&uuml;gen</trd>
  <trd lng="en">lie</trd>
  <trd lng="fr">mentir</trd>
  <trd lng="hu">hazudik</trd>  
  <trd lng="nl">liegen</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_g;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_r;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="mensog.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Intence dirita malvera&jcirc;o por trompi iun.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_l;&c_u;&c_s;&c_mol;&c_n;&c_ja;</trd>,
    <trd>&c_m;&c_a;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>L&uuml;ge</trd>,
      <trd>Unwahrheit</trd>
  </trdgrp>
  <trd lng="hu">hazugs&aacute;g</trd>  
</drv>

</art>
