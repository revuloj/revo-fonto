<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: mensog.xml,v 1.33 2021/05/31 05:43:54 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>mensog</rad>/i
    </kap>

    <drv mrk="mensog.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <snc mrk="mensog.0i.KOMUNE">
        <gra><vspec>x</vspec></gra>
        <dif>
          Intence diri malvera&jcirc;on:
          <ekz>
            senhonte <tld/>i<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>i ma&scirc;ine<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>i kiel kalendaro<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>i kiel funebra parolo<fnt>Z</fnt>;
          </ekz><ekz>
            <ind><tld/>i kiel gazeto</ind>
            <trd lng="fr">mentir comme un arracheur de dents</trd>
            <fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>a atestanto<fnt>Z</fnt>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            <tld/>i indiferentecon
            <klr>(trompe &scirc;ajnigi)</klr>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            tiu &ccirc;ambro <tld/>is komforton.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="sxajn.0igi">&scirc;ajnigi</ref>
        </refgrp>
      </snc>
  <trdgrp lng="be">
        <trd>&c_h;&c_l;&c_u;&c_s;&c_ib;&c_c;&c_mol;</trd>,
        <trd>&c_m;&c_a;&c_n;&c_ib;&c_c;&c_mol;</trd>,
        <trd>&c_ib;&c_l;&c_g;&c_a;&c_c;&c_mol;</trd>
      </trdgrp>
  <trd lng="bo">&#xF66;&#xF90;&#xFB1;&#xF42;&#xF0B;&#xF62;&#xFAB;&#xF74;&#xF53;&#xF0B;&#xF56;&#xF64;&#xF51;</trd>
  <trdgrp lng="br">
        <trd>gaouiadi&ntilde;</trd>,
        <trd>konta&ntilde; <ind>gevier</ind></trd>
      </trdgrp>
  <trd lng="ca">mentir</trd>
  <trd lng="cs">lh&aacute;t</trd>
  <trd lng="de">l&uuml;gen</trd>
  <trd lng="en">lie</trd>
  <trd lng="es">mentir</trd>
  <trd lng="fr">mentir</trd>
  <trd lng="hu">hazudik</trd>
  <trd lng="id">ber<ind>bohong</ind></trd>
  <trd lng="it">mentire</trd>
  <trd lng="nl">liegen</trd>
  <trd lng="oc">mentir</trd>
  <trd lng="pl">k&lstroke;ama&cacute;</trd>
  <trd lng="pt">mentir</trd>
  <trdgrp lng="ru">
        <trd>&c_l;&c_g;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_v;&c_r;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
  <trdgrp lng="sk">
    <trd>cig&aacute;ni&tcaron;</trd>,
    <trd>luha&tcaron;</trd>
  </trdgrp>
</drv>

    <drv mrk="mensog.0o">
      <kap><tld/>o</kap>
      <snc mrk="mensog.0o.KOMUNE">
        <dif>
          Intence dirita malvera&jcirc;o por trompi iun:
          <ekz>
            en la bur&gcirc;aj gazetoj kaj libroj estas nur <tld/>oj
            <fnt><bib>InfanTorent2</bib>,
              <lok>unua parto, &ccirc;apitro 1a</lok></fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="af">leuen</trd>
  <trd lng="am">&#x12CD;&#x1238;&#x1275;</trd>
  <trd lng="ar">&#x643;&#x630;&#x628;&#x629;</trd>
  <trdgrp lng="be">
        <trd>&c_h;&c_l;&c_u;&c_s;&c_mol;&c_n;&c_ja;</trd>,
        <trd>&c_m;&c_a;&c_n;&c_a;</trd>
      </trdgrp>
  <trd lng="br">gaou</trd>
  <trd lng="ca">mentida</trd>
  <trd lng="cs">le&zcaron;</trd>
  <trdgrp lng="de">
        <trd>L&uuml;ge</trd>,
        <trd>Unwahrheit</trd>
      </trdgrp>
  <trd lng="es">mentira</trd>
  <trd lng="eu">gezur</trd>
  <trd lng="fr">mensonge</trd>
  <trd lng="gl">mentira</trd>
  <trd lng="hu">hazugs&aacute;g</trd>
  <trd lng="id">ke<ind>bohong</ind>an</trd>
  <trd lng="it">bugia</trd>
  <trd lng="ne">&#x91D;&#x942;&#x91F;</trd>
  <trd lng="nl">leugen</trd>
  <trd lng="oc">bala</trd>
  <trd lng="pl">k&lstroke;amstwa</trd>
  <trd lng="pt">mentira</trd>
  <trd lng="ru">&c_l;&c_o;&c_zh;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>lo&zcaron;</trd>,
    <trd>luhanie</trd>,
    <trd>luhanina</trd>
  </trdgrp>
  <trd lng="sq">g&#xEB;njesht&#xEB;r</trd>
</drv>

    <drv mrk="mensog.0a">
      <kap><tld/>a</kap>
      <snc>
        <dif>
          Bazita sur <tld/>o a&ubreve; prezentanta <tld/>on:
          <ekz>
            <ind>sen <tld/>a</ind> rekomendo ne iros la vendo
            <fnt><bib>PrV</bib></fnt>.
            <trd lng="ru">&c_n;&c_je; &c_o;&c_b;&c_m;&c_a;&c_n;&c_je;&c_sh;&c_mol; &#x2010; &c_n;&c_je; &c_p;&c_r;&c_o;&c_d;&c_a;&c_sh;&c_mol;</trd>
          </ekz>
        </dif>
      </snc>
  <trdgrp lng="cs">
    <trd>lhav&yacute;</trd>,
    <trd>lh&aacute;&rcaron;sk&yacute;</trd>,
    <trd>prolhan&yacute;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>k&lstroke;amliwy</trd>,
    <trd>fa&lstroke;szywy</trd>
  </trdgrp>
  <trd lng="ro">fals</trd>
  <trdgrp lng="ru">
        <trd>&c_l;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>,
        <trd>&c_l;&c_zh;&c_i;&c_v;&c_y;&c_j;</trd>
      </trdgrp>
  <trdgrp lng="sk">
    <trd>klamliv&yacute;</trd>,
    <trd>klam&aacute;rsky</trd>
  </trdgrp>
</drv>

    <drv mrk="mensog.0ema">
      <kap><tld/>ema</kap>
      <snc mrk="mensog.0ema.KOMUNE">
        <dif>
          Volonte diranta intencajn malvera&jcirc;ojn:
          <ekz>
            <tld/>ema lango
            <fnt><bib>MT</bib>, <lok>&Psa; 109:2</lok></fnt>;
          </ekz><ekz>
            finegocistoj, <tld/>emaj episkopoj, naciemaj generaloj
            k<klr>aj</klr> aliaj altranguloj
            <fnt><bib>Lanti</bib>,
              <lok>publika letero al Romen Rolan</lok></fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="br">gaouiat</trd>
  <trdgrp lng="cs">
    <trd>lhav&yacute;</trd>,
    <trd>lh&aacute;&rcaron;sk&yacute;</trd>,
    <trd>prolhan&yacute;</trd>
  </trdgrp>
  <trd lng="de">l&uuml;gnerisch</trd>
  <trd lng="es">mentiroso, -a<klr> (adj.)</klr></trd>
  <trd lng="fr">menteur<klr tip="amb"> (adj.)</klr></trd>
  <trdgrp lng="pl">
        <trd>k&lstroke;amliwy</trd>,
        <trd>zak&lstroke;amany</trd>
      </trdgrp>
  <trd lng="ru">&c_l;&c_zh;&c_i;&c_v;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>klamliv&yacute;</trd>,
    <trd>klam&aacute;rsky</trd>
  </trdgrp>
</drv>

    <drv mrk="mensog.0ulo">
      <kap><tld/>ulo</kap>
      <snc mrk="mensog.0ulo.KOMUNE">
        <dif>
          Homo, kiu ofte diras intencajn malvera&jcirc;ojn:
          <ekz>
            li hontis, ke li &scirc;ajnis <tld/>ulo, kaj la parencoj priridis
            lin
            <fnt><bib>ElektFab</bib>,
              <lok>tablo &leftquot;kovru vin&rightquot;, la orazeno kaj bastono el
              sako</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
  <trdgrp lng="br">
        <trd>gaouiad</trd>,
        <trd>gaouier</trd>
      </trdgrp>
  <trdgrp lng="cs">
    <trd>lh&aacute;&rcaron;</trd>,
    <trd>prolhan&yacute; &ccaron;l.</trd>
  </trdgrp>
  <trd lng="de">L&uuml;gner</trd>
  <trd lng="es">mentiroso, -a<klr> (subst.)</klr></trd>
  <trd lng="fr">menteur<klr tip="amb"> (subst.)</klr></trd>
  <trd lng="id">pem<ind>bohong</ind></trd>
  <trd lng="pl">k&lstroke;amca</trd>
  <trd lng="ru">&c_l;&c_zh;&c_je;&c_c;</trd>
  <trd lng="sk">klam&aacute;r</trd>
</drv>
  </art>
