<drv mrk="mensog.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Bazita sur <tld/>o a&ubreve; prezentanta <tld/>on:
      <ekz>
        <tld/>a atestanto
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 21:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne atentu <tld/>ajn vortojn
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 5:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> profetoj <klr>[&#x2026;]</klr> faras al ili <tld/>ajn anta&ubreve;dirojn
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 22:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        signoj de <tld/>aj &scirc;iaj larmoj
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        nia aserto estas naive-<tld/>a
        <fnt>
          <bib>Ret</bib>
          <lok>Aldono</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>sen <tld/>a</ind> rekomendo ne iros la vendo
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="ru">&c_n;&c_je; &c_o;&c_b;&c_m;&c_a;&c_n;&c_je;&c_sh;&c_mol; &#x2010; &c_n;&c_je;
        &c_p;&c_r;&c_o;&c_d;&c_a;&c_sh;&c_mol;</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hipokr.0a">hipokrita</ref>,
      <ref cel="tromp.0a">trompa</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_l;&c_u;&c_s;&c_mol;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_ib;&c_l;&c_zh;&c_y;&c_v;&c_y;</trd>,
    <trd>&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_y;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>lhav&yacute;</trd>,
    <trd>lh&aacute;&rcaron;sk&yacute;</trd>,
    <trd>prolhan&yacute;</trd>
  </trdgrp>
  <trd lng="de">falsch <klr>(l&uuml;gend)</klr></trd>
  <trdgrp lng="en">
    <trd>mendacious</trd>,
    <trd>false</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5618;&#x306E;  <pr>&#x3046;&#x305D;&#x306E;</pr></trd>,
    <trd>&#x507D;&#x308A;&#x306E;  <pr>&#x3044;&#x3064;&#x308F;&#x308A;&#x306E;</pr></trd>,
    <trd>&#x865A;&#x507D;&#x306E;  <pr>&#x304D;&#x3087;&#x304E;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
