<drv mrk="mensog.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Bazita sur <tld/>o a&ubreve; prezentanta <tld/>on:
      <ekz>
        <ind>sen <tld/>a</ind> rekomendo ne iros la vendo
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="ru">&c_n;&c_je; &c_o;&c_b;&c_m;&c_a;&c_n;&c_je;&c_sh;&c_mol; &#8208; &c_n;&c_je;
        &c_p;&c_r;&c_o;&c_d;&c_a;&c_sh;&c_mol;</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>lhav&yacute;</trd>,
    <trd>lh&aacute;&rcaron;sk&yacute;</trd>,
    <trd>prolhan&yacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>mendacious</trd>,
    <trd>false</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>k&lstroke;amliwy</trd>,
    <trd>fa&lstroke;szywy</trd>
  </trdgrp>
  <trd lng="ro">fals</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_l;&c_zh;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>klamliv&yacute;</trd>,
    <trd>klam&aacute;rsky</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_r;&c_je;&c_h;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_r;&c_a;&c_v;&c_d;&c_i;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="mensog.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="mensog.0ema.KOMUNE">
    <dif>
      Volonte diranta intencajn malvera&jcirc;ojn:
      <ekz>
        <tld/>ema lango
        <fnt><bib>MT</bib>, <lok>&Psa; 109:2</lok></fnt>;
      </ekz><ekz>
        finegocistoj, <tld/>emaj episkopoj, naciemaj generaloj
        k<klr>aj</klr> aliaj altranguloj
        <fnt><bib>Lanti</bib>,
          <lok>publika letero al Romen Rolan</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">gaouiat</trd>
  <trdgrp lng="cs">
    <trd>lhav&yacute;</trd>,
    <trd>lh&aacute;&rcaron;sk&yacute;</trd>,
    <trd>prolhan&yacute;</trd>
  </trdgrp>
  <trd lng="de">l&uuml;gnerisch</trd>
  <trd lng="en">mendacious</trd>
    <trd lng="es">mentiroso, -a<klr> (adj.)</klr></trd>
  <trd lng="fr">menteur<klr tip="amb"> (adj.)</klr></trd>
  <trdgrp lng="pl">
    <trd>k&lstroke;amliwy</trd>,
    <trd>zak&lstroke;amany</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_zh;&c_i;&c_v;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>klamliv&yacute;</trd>,
    <trd>klam&aacute;rsky</trd>
  </trdgrp>
  <trd lng="uk">&c_b;&c_r;&c_je;&c_h;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>
</drv>

<drv mrk="mensog.0ulo">
  <kap><tld/>ulo, <var><kap><tld/>isto</kap></var></kap>
  <snc mrk="mensog.0ulo.KOMUNE">
    <dif>
      Homo, kiu ofte diras intencajn malvera&jcirc;ojn:
      <ekz>
        li estas <tld/>isto kaj malnoblulo
        <fnt>
          <bib>F</bib>
          <lok>&FE; 37</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili jam en la tuta urbo estas konataj kiel <tld/>istoj
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        sor&ccirc;istoj kaj idolanoj kaj &ccirc;iuj <tld/>uloj, ilia lota&jcirc;o estos <klr>[&#8230;]</klr>
        brulanta per fajro kaj sulfuro
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 21:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        gento de <tld/>uloj! ekkriis la tajloro, unu same sendia kaj malzorga, kiel alia
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tablo , la orazeno kaj bastono el sako</lok>
        </fnt>!
      </ekz>
      <ekz>
        li hontis, ke li &scirc;ajnis <tld/>ulo, kaj la parencoj priridis
        lin
        <fnt><bib>ElektFab</bib>,
          <lok>tablo &leftquot;kovru vin&rightquot;, la orazeno kaj bastono el
          sako</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Brown</nom> kaj <nom>Morris</nom> salutis sian amikon, kaj repleniginte la glasojn, toastis la
        sanon de &leftquot;la plej granda <tld/>isto en la mondo&rightquot;
        <fnt>
          <bib>TAA</bib>
          <lok>&Ccirc;apitro II.</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxarla.0o">&ccirc;arlatano</ref>,
      <ref cel="tromp.0isto">trompisto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="br">
    <trd>gaouiad</trd>,
    <trd>gaouier</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>lh&aacute;&rcaron;</trd>,
    <trd>prolhan&yacute; &ccaron;l.</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd lng="de">L&uuml;gner</trd>,
    <trd>Blender</trd>
  </trdgrp>
  <trd lng="en">liar</trd>
  <trd lng="es">mentiroso, -a<klr> (subst.)</klr></trd>
  <trd lng="fr">menteur<klr tip="amb"> (subst.)</klr></trd>
  <trd lng="id">pem<ind>bohong</ind></trd>
  <trd lng="pl">k&lstroke;amca</trd>
  <trd lng="ru">&c_l;&c_zh;&c_je;&c_c;</trd>
  <trd lng="sk">klam&aacute;r</trd>
  <trd lng="uk">&c_b;&c_r;&c_je;&c_h;&c_u;&c_n;</trd>
</drv>
