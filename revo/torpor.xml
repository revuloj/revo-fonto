<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: torpor.xml,v 1.3 2015/07/04 08:37:45 revo Exp $">
<kap>
  <rad>torpor</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="torpor.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="torpor.0o.KOMUNE">
    <dif>
      Korpa kaj anima malvigleco, sensenteco;
      <ref tip="vid" cel="letarg.0o.FIG">letargio<sncref/></ref>,
      <ref tip="vid" cel="somnol.0o.KOMUNE">somnolo</ref>:
      <ekz>
        &scirc;i sen<tld/>i&gcirc;is
        <fnt>
          <aut>V. Catal&agrave;, trad. J. Ventura i Freixas</aut>,
          <vrk>Soleco</vrk>,
          <lok>1967</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>chabost</trd>,
    <trd>netečnost</trd>,
    <trd>ochablost</trd>,
    <trd>torpor</trd>,
    <trd>tupost</trd>
  </trdgrp>
  <trd lng="fr">torpeur</trd>
  <trdgrp lng="sk">
    <trd>ochabnutosť</trd>,
    <trd>tuposť</trd>
  </trdgrp>
</drv>
</art>

