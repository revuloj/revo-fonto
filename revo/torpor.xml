<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: torpor.xml,v 1.9 2025/04/07 05:23:34 revo Exp $">
<kap>
  <rad>torpor</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="torpor.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="torpor.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      &Ccirc;e malgrandaj mamuloj kaj birdoj, stato karaterizita de draste reduktita
      metabolo kaj senaktiveco, kutime reage al ekstremaj medikondi&ccirc;oj kiel malvarmo.
    </dif>
    <trd lng="be">&c_a;&c_n;&c_a;&c_b;&c_ib;&c_jo;&c_z;</trd>
    <trdgrp lng="de">
      <trd>Torporo</trd>,
      <trd>K&auml;lteschlaf</trd>,
      <trd>Verklammern</trd>
    </trdgrp>
    <trd lng="en">torpor</trd>
  </snc>
  <snc mrk="torpor.0o.KOMUNE">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Korpa kaj anima malvigleco, sensenteco:
      <ekz>
        la furiozo <tld/>is en li kaj li blasfemis dum la tuta tago
        <fnt>
          <bib>LGA</bib>
          <lok>III.</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i sen<tld/>i&gcirc;is
        <fnt>
          <aut>V. Catal&agrave;, trad. J. Ventura i Freixas</aut>,
          <vrk>Soleco</vrk>,
          <lok>1967</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rigid.0igxi">rigidi&gcirc;i</ref>,
      <ref cel="sxton.0igxi.FIG">&scirc;toni&gcirc;i<sncref/></ref>,
      <ref cel="letarg.0o.FIG">letargio<sncref/></ref>,
      <ref cel="somnol.0o.KOMUNE">somnolo</ref>,
      <ref cel="stupor.0o">stuporo</ref>
    </refgrp>
  <trdgrp lng="be">
    <trd>&c_a;&c_n;&c_ja;&c_m;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_z;&c_d;&c_r;&c_a;&c_n;&c_c;&c_mol;&c_v;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="de">Erstarrung</trd>
    <trdgrp lng="zh">
    <trd>&#20923;&#20725; <pr>d&#242;ngji&amacron;ng</pr></trd>,
    <trd>&#20941;&#27565; <pr>d&#242;ngji&amacron;ng</pr></trd>
  </trdgrp>
</snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>malvigleco</ke>, <ke>malagemo</ke>, <ke>dormemo</ke>.
  </rim>

  <trdgrp lng="cs">
    <trd>chabost</trd>,
    <trd>nete&ccaron;nost</trd>,
    <trd>ochablost</trd>,
    <trd>torpor</trd>,
    <trd>tupost</trd>
  </trdgrp>
  <trd lng="fr">torpeur</trd>
  <trdgrp lng="pl">
    <trd>odr&eogonek;twienie</trd>,
    <trd>os&lstroke;upienie</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ochabnutos&tcaron;</trd>,
    <trd>tupos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&#1086;&c_r;&#1087;&#1086;&c_r;</trd>,
    <trd>&c_z;&c_a;&c_c;&c_ib;&#1087;&c_je;&#1085;&c_ib;&#1085;&#1085;&#1103;</trd>,
    <trd>&c_g;&#1083;&c_i;&c_b;&#1086;&#1082;&c_je; &#1087;&c_r;&c_i;&c_g;&#1085;&c_ib;&c_ch;&c_je;&#1085;&#1085;&#1103;
    &c_s;&c_v;&c_ib;&c_d;&#1086;&#1084;&#1086;&c_s;&c_t;&c_ib;.</trd>
  </trdgrp>
</drv>
</art>

