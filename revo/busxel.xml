<art mrk="$Id: busxel.xml,v 1.20 2021/03/05 18:27:14 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bu&scirc;el</rad>/o
</kap>
<drv mrk="busxel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="busxel.0o.MEZ">
    <dif>
      Malnova komunuza <ref tip="lst" cel="unu.mezur0o" lst="voko:kvantoj">mezurunuo</ref> de
      kapacito, por grenoj kaj &scirc;uteblaj materialoj; proksimume
      egala al 
      36 <ref cel="litr.0o">litroj</ref> en Britujo kaj Kanado, 10
      litroj en 
      Francujo, 55 litroj en Prusujo, ktp.:
      <ekz>
        ni a&ccirc;etis <tld/>on da papriko
        <fnt>
          <aut>Stela</aut>,
          <vrk><url ref="http://verdavojeto.bravejournal.com/">
           Verda vojeto de Stela</url></vrk>,
          <lok>2007-09-08</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>                       
  <trd lng="be">&c_b;&c_u;&c_sh;&c_a;&c_l;&c_mol;</trd>
  <trd lng="br">poezell</trd>
  <trd lng="de">Scheffel</trd>
  <trd lng="en">bushel</trd>
  <trdgrp lng="he">
    <trd>&#x5D1;&#x5D5;&#x5E9;&#x5DC;</trd>,
    <trd>&#x5DE;&#x5D9;&#x5D3;&#x5EA; &#x5E0;&#x5E4;&#x5D7; &#x5DB;-36 &#x5DC;&#x5D9;&#x5D8;&#x5E8;</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>bushel</trd>, <trd>staio<klr> (mis.)</klr></trd>
  </trdgrp>
  <trd lng="fr">boisseau</trd>
  <trd lng="hu">v&eacute;ka</trd>
  <trd lng="pl">buszel</trd>
  <trd lng="pt">alqueire</trd>
  <trd lng="ru">&c_b;&c_u;&c_sh;&c_je;&c_l;&c_mol;</trd>
</drv>
</art>
