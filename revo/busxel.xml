<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>bu&scirc;el</rad>/o
</kap>
<drv mrk="busxel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Malnova komunuza <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de
      kapacito, por grenoj kaj &scirc;uteblaj materialoj; proksimume egala al 
      36 <ref cel="litr.0o">litroj</ref> en Britujo kaj Kanado, 10 litroj en 
      Francujo, 55 litroj en Prusujo, ktp.
    </dif>
  </snc>                       
  <trd lng="be">&c_b;&c_u;&c_sh;&c_a;&c_l;&c_mol;</trd>
  <trd lng="br">poezell</trd>
  <trd lng="de">Scheffel</trd>
  <trd lng="en">bushel</trd>
  <trd lng="fr">boisseau</trd>
  <trd lng="hu">v&eacute;ka</trd>
  <trd lng="ru">&c_b;&c_u;&c_sh;&c_je;&c_l;&c_mol;</trd>
</drv>
</art>
