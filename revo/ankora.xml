<drv mrk="ankora.0a">
  <kap><tld/>a</kap>
  <snc mrk="ankora.0a.KOMUNE">
    <dif>
      <ref tip="dif" cel="plu.0a">Plua</ref>, da&ubreve;riganta, aldonata:
      <ekz>
        ni <klr>[&#x2026;]</klr> verkis <tld/>an strofon
        <fnt>
          <aut>L. Ganman</aut>,
          <vrk><url ref="http://www.linusganman.se/j8j.htm">
          &Gcirc;oja</url></vrk>,
          <lok>2015</lok>
        </fnt>;
      </ekz><ekz>
        kion pensi pri la <tld/>a diskriminacio kontra&ubreve; gejaj kaj
        lesbaj fiduloj, kion pensi pri la <tld/>a malpermeso uzi kondomojn
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="&Monato;/2001/006823.html">Opinio: Tro Senkosta
          Pentemo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="pli.0a.aldona">plia<sncref/></ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_d;&c_a;&c_t;&c_k;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_d;&c_a;&c_l;&c_je;&c_j;&c_sh;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><klr>(ein) </klr> weiterer</trd>,
    <trd>andauernde</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>additional</trd>,
    <trd>further</trd>,
    <trd>more <klr>(additional)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>suplementario</trd>,
    <trd>adicional</trd>,
    <trd>de m&aacute;s</trd>,
    <trd>de una vez m&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>subsistant</trd>
    <trd>suppl&eacute;mentaire</trd>
  </trdgrp>
