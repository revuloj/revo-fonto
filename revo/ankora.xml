<drv mrk="ankora.0a">
  <kap><tld/>a</kap>
  <snc mrk="ankora.0a.KOMUNE">
    <dif>
      Plua, da&ubreve;riganta, aldonata:
      <ekz>
        ni <klr>...</klr> verkis <tld/>an strofon
        <fnt>
          <aut>L. Ganman</aut>,
          <vrk><url ref="http://www.linusganman.se/j8j.htm">
          &Gcirc;oja</url></vrk>,
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        kion pensi pri la <tld/>a diskriminacio kontra&ubreve; gejaj kaj lesbaj fiduloj, kion pensi pri la ~a malpermeso uzi kondomojn
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://www.esperanto.be/fel/mon/006823.html">Opinio: Tro senkosta pentemo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de"><klr>(ein) </klr> weiterer</trd>
  <trdgrp lng="es">
    <trd>suplementario</trd>,
    <trd>adicional</trd>,
    <trd>de m&aacute;s</trd>,
    <trd>de una vez m&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>subsistant</trd>
    <trd>suppl&eacute;mentaire</trd>
  </trdgrp>
</drv>
