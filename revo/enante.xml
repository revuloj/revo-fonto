<art mrk="$Id: enante.xml,v 1.17 2024/08/12 21:26:03 revo Exp $">
<kap>
  <rad>enantem</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="enante.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Surmukmembrana <ref tip="super" cel="erupci.0o.MED">erupcio</ref>:
      <ekz>
        <tld/>o esta simptomo &ccirc;e skarlatino, morbilo, sifiliso, rubeolo, sed povas okazi
        anka&ubreve; &ccirc;e kronvirusa infekto.
      </ekz>
    </dif>
    <ref tip="vid" cel="ekzant.0o">ekzantemo</ref>
  </snc>
  <trd lng="be">&c_e;&c_n;&c_a;&c_n;&c_t;&c_e;&c_m;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>enant&eacute;m</trd>,
    <trd>vyr&aacute;&zcaron;ka na sliznici</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schleimhautausschlag</trd>,
    <trd>Enanthem</trd>
  </trdgrp>
  <trd lng="fr">&eacute;nanth&egrave;me</trd>
  <trdgrp lng="hu">
    <trd>ny&aacute;lkah&aacute;rtya-ki&uuml;t&eacute;s</trd>,
    <trd>enanthema</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#31896;&#33180;&#30137; <pr>&#12397;&#12435;&#12414;&#12367;&#12375;&#12435;</pr></trd>,
    <trd>&#20869;&#30137; <pr>&#12394;&#12356;&#12375;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="pl">wysypka</trd>
  <trd lng="pt">enantemo</trd>
  <trd lng="ru">&c_e;&c_n;&c_a;&c_n;&c_t;&c_je;&c_m;&c_a;</trd>
  <trd lng="sk">vyr&aacute;&zcaron;ka na sliznici</trd>
  <trd lng="uk">&c_je;&c_n;&c_a;&c_n;&c_t;&c_je;&c_m;&c_a;</trd>
</drv>
</art>
