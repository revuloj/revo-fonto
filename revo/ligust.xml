<kap>
  <rad>ligustr</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ligust.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="ligust.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbedo kun bonodoraj blankaj floroj;
      <ref tip="lst" cel="genr.0o" lst="voko:botanikaj_genroj">genro</ref>
      el familio <ref tip="malprt" cel="oleac.0oj">oleacoj</ref>
      (<trd lng="la">Ligustrum</trd>):
      <ekz>
        densejoj el <tld/>oj kaj la&ubreve;rarboj
        <fnt>
          <aut>E. About, trad. G. Moch</aut>
          <vrk><url ref="&Gutenberg;files/65835/65835-0.txt">La Re&gcirc;o de la Montoj</url></vrk>
          <lok>1909</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ib;&c_r;&c_u;&c_ch;&c_y;&c_n;&c_a;</trd>
  <trd lng="bg">&c_l;&c_u;&c_g;&c_u;&c_s;&c_t;&c_r;&c_u;&c_m;</trd>
  <trd lng="de">Liguster</trd>
  <trd lng="en">ligustrum</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&a_kasra;&a_n;&a_d;&a_A;&a_r;&f_ch;&a_h;</trd>,
    <trd>&a_b;&a_r;&f_g; &a_n;&a_w;</trd>
  </trdgrp>
  <trd lng="fr">tro&egrave;ne</trd>
  <trd lng="hu">fagyal</trd>
  <trd lng="ja">&#x30A4;&#x30DC;&#x30BF;&#x30CE;&#x30AD;</trd>
  <trd lng="nl">liguster</trd>
  <trd lng="pl">ligustr</trd>
  <trd lng="pt">ligustro</trd>
  <trd lng="ro">lemn cainesc</trd>
  <trd lng="ru">&c_b;&c_i;&c_r;&c_ju;&c_ch;&c_i;&c_n;&c_a; <klr>(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x431;&#x438;&#x440;&#x44E;&#x447;&#x438;&#x43D;&#x430;</trd>,
    <trd>&#x432;&#x43E;&#x432;&#x447;&#x456; &#x44F;&#x433;&#x43E;&#x434;&#x438;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5973;&#x8C9E; <pr>n&#x1D4;:zh&#x113;n</pr></trd>,
    <trd>&#x5973;&#x8D1E; <pr>n&#x1D4;:zh&#x113;n</pr></trd>,
    <trd>&#x5375;&#x53F6;&#x5973;&#x8D1E; <pr>lu&#x1CE;ny&#xE8;r&#x1D4;zh&#x113;n</pr></trd>,
    <trd>&#x5375;&#x8449;&#x5973;&#x8C9E; <pr>lu&#x1CE;ny&#xE8;r&#x1D4;zh&#x113;n</pr></trd>,
    <trd>&#x5973;&#x8C9E; <pr>n&#x1DA;zh&#x113;n</pr></trd>,
    <trd>&#x5973;&#x8D1E; <pr>n&#x1DA;zh&#x113;n</pr></trd>
  </trdgrp>
</drv>
