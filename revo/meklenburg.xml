  <art mrk="$Id: meklenburg.xml,v 1.9 2024/01/25 20:10:36 revo Exp $">
    <kap>
      <rad>Meklenburg</rad>/o,
      <var><kap><rad var="i">Meklenburgi</rad>/o</kap></var>
    </kap>

<drv mrk="meklenburg.0o">
  <kap><tld/>o,
    <var><kap><tld var="i"/>o</kap></var>
  </kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
    <dif>
      Nordgermana regiono sude de Balta Maro kaj
      okcidente de <ref cel="pomeri.0o">Pomerio</ref>:
      <ekz>
        vi estas grafo de <nom>Brand</nom>, vi venas el <tld/>o, mi
        estas via servanto
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz><ref cel="pomeri.MeklenburgoAntau0o">Meklenburgo-Anta&ubreve;pomerio</ref>;</ekz>
      <ekz>
        en <tld var="i"/>o kaj Anglio li provis solidigi sian financan kaj
        profesian vivon
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2010/010141.php">Homo universala, homo moderna: Johann Joachim Becher
              naski&gcirc;is anta&ubreve; 375 jaroj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_M;&c_e;&c_k;&c_l;&c_je;&c_n;&c_b;&c_u;&c_r;&c_g;</trd>
    <trd lng="de">Mecklenburg</trd>
    <trd lng="fr">Mecklembourg</trd>
    <trd lng="pl">Meklemburgia</trd>     
    <trd lng="ro">Mecklenburg</trd>
  </snc>
  <trd lng="ja">&#x30E1;&#x30AF;&#x30EC;&#x30F3;&#x30D6;&#x30EB;&#x30AF;&#x5730;&#x65B9;  <pr>&#x30E1;&#x30AF;&#x30EC;&#x30F3;&#x30D6;&#x30EB;&#x30AF;&#x3061;&#x307B;&#x3046;</pr></trd>
  <trdgrp lng="zh">
    <trd>&#x6885;&#x514B;&#x4F26;&#x5821; <pr>m&#xE9;ik&#xE8;l&#xFA;nb&#x1CE;o</pr></trd>,
    <trd>&#x6885;&#x514B;&#x502B;&#x5821; <pr>m&#xE9;ik&#xE8;l&#xFA;nb&#x1CE;o</pr></trd>
  </trdgrp>
</drv>

