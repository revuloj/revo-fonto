<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: merkatik.xml,v 1.14 2026/02/18 17:19:45 revo Exp $">
<kap>
  <rad>merkatik</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="merkatik.0o">
  <kap><tld/>o</kap>
  <snc mrk="merkatik.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Studo de <ref tip="vid" cel="merkat.0o.EKON">merkatoj</ref>
      por adapti komerca&jcirc;ojn kaj servojn al a&ccirc;etantoj:
      <ekz>
        mi estas &jcirc;urnalisto kaj specialisto pri <tld/>o
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Monato;/2003/008035.html">Evgeni Georgiev</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        la projekto estas ruza <tld/>a instrumento, per kiu okcidentaj industri-landoj
        volas akapari la merkaton de la tria mondo sub mantelo de altruismo
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2006/008867.php">Primitive</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        politikaj respondeculoj alprenas <tld/>ajn teknikojn por
        transformi&gcirc;i mem en produktojn
        <fnt>
          <aut>B. Stiegler, trad. J. Hasbun</aut>,
          <vrk><url ref="&Mondediplo;article918.html">
          La deziro asfiksiata...</url></vrk>,
          <lok>le Monde diplomatique, 2004-05</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_r;&c_k;&c_je;&c_t;&c_y;&c_n;&c_g;</trd>,
    <trd>&c_d;&c_a;&c_s;&c_mol;&c_l;&c_je;&c_d;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je; &c_r;&c_y;&c_n;&c_k;&c_u;</trd>
  </trdgrp>
  <trd lng="ca">m&agrave;rqueting</trd>
  <trd lng="cs">nauka o trhu</trd>
  <trdgrp lng="de">
    <trd>Marktforschung</trd>,
    <trd>Marketing</trd>
  </trdgrp>
  <trd lng="en">marketing</trd>
  <trdgrp lng="es">
    <trd>mercadotecnia</trd>,
    <trd>marketing</trd>
  </trdgrp>
  <trd lng="fa">&a_b;&a_A;&a_z;&a_A;&a_r;&f_p;&f_zh;&a_w;&a_h;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>marketing</trd>,
    <trd>mercatique</trd>
  </trdgrp>
  <trd lng="pl">marketing</trd>
  <trd lng="sk">n&aacute;uka o trhu</trd>
  <trd lng="uk">&c_m;&c_a;&c_r;&c_k;&c_je;&c_t;&c_i;&c_n;&c_gu;</trd>
  <trdgrp lng="zh">
    <trd>&#24066;&#22330;&#35843;&#26597; <pr>sh&igrave;ch&#462;ngdi&agrave;och&aacute;</pr></trd>,
    <trd>&#24066;&#22580;&#35519;&#26597; <pr>sh&igrave;ch&#462;ngdi&agrave;och&aacute;</pr></trd>,
    <trd>&#24066;&#22330;&#33829;&#38144; <pr>sh&igrave;ch&#462;ngy&iacute;ngxi&amacron;o</pr></trd>,
    <trd>&#24066;&#22580;&#29151;&#37559; <pr>sh&igrave;ch&#462;ngy&iacute;ngxi&amacron;o</pr></trd>,
    <trd>&#29151;&#37559; <pr>y&iacute;ngxi&amacron;o</pr></trd>,
    <trd>&#33829;&#38144; <pr>y&iacute;ngxi&amacron;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="merkatik.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="merkatik.0isto.EKON">
    <dif>
      <ref tip="dif" cel="merkat.0isto.EKON">Merkatisto</ref>:
      <ekz>
        bona <tld/>isto kredas, ke la firmao devas servi la klientaron
        <fnt>
          <aut>D. Keefe</aut>,
          <vrk><url ref="http://www.liberafolio.org/2007/merkatigo">
          merkatigo</url></vrk>,
          <lok>libera folio, 2007-09-25</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_r;&c_k;&c_je;&c_t;&c_o;&c_l;&c_ja;&c_g;</trd>,
    <trd>&c_d;&c_a;&c_s;&c_mol;&c_l;&c_je;&c_d;&c_ch;&c_y;&c_k; &c_r;&c_y;&c_n;&c_k;&c_u;</trd>
  </trdgrp>
  <trd lng="ca">agent de m&agrave;rqueting</trd>
  <trdgrp lng="de">
    <trd>Marktforscher</trd>,
    <trd>Marketingexperte</trd>
  </trdgrp>
  <trd lng="en">marketer</trd>
  <trd lng="es">analista de mercado</trd>
  <trdgrp lng="fr">
    <trd>marketiste</trd>,
    <trd>mercatiste</trd>
  </trdgrp>
  <trd lng="pl">analityk rynku</trd>
  <trdgrp lng="zh">
    <trd>&#24066;&#22330;&#30740;&#31350;&#20154;&#21592; <pr>sh&igrave;ch&#462;ngy&aacute;nji&umacron;r&eacute;nyu&aacute;n</pr></trd>,
    <trd>&#24066;&#22580;&#30740;&#31350;&#20154;&#21729; <pr>sh&igrave;ch&#462;ngy&aacute;nji&umacron;r&eacute;nyu&aacute;n</pr></trd>,
    <trd>&#24066;&#22330;&#35843;&#26597;&#20154;&#21592; <pr>sh&igrave;ch&#462;ngdi&agrave;och&aacute;r&eacute;nyu&aacute;n</pr></trd>,
    <trd>&#24066;&#22580;&#35519;&#26597;&#20154;&#21729; <pr>sh&igrave;ch&#462;ngdi&agrave;och&aacute;r&eacute;nyu&aacute;n</pr></trd>
  </trdgrp>
 </drv>
</art>

