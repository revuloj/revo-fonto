      <trd>&c_p;&c_a;&c_sh;&c_y;&c_r;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd><baz>lapang</baz>melapangkan</trd>,
      <trd><baz>lapang</baz>memperlapangkan</trd>,
      <trd><baz>lapang</baz>memperlapang</trd>,
      <trd><baz>lebar</baz>memperlebar</trd>,
      <trd><baz>lebar</baz>melebarkan</trd>,
      <trd><baz>lega</baz>melegakan</trd>,
      <trd><baz>lega</baz>memperlega</trd>,
      <trd><baz>luas</baz>memperluas</trd>,
      <trd><baz>luas</baz>meluaskan</trd>,
      <trd><baz>sebar</baz>menyebarkan</trd>,
      <trd><baz>besar</baz>memperbesar</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>szerzy&cacute;</trd>,
      <trd>poszerzy&cacute;</trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_a;&c_s;&c_sh;&c_i;&c_r;&c_ja;&c_t;&c_mol;</trd>
    <trdgrp lng="tr">
      <trd>geni&scedil;letmek</trd>,
      <trd>b&uuml;y&uuml;tmek</trd>
    </trdgrp>
