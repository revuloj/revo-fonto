<drv mrk="vast.0ega">
  <kap><tld/>ega</kap>
  <snc mrk="vast.0ega.KOMUNE">
    <dif>
      Tre ampleksa, tre etendita:
      <ekz>
        balda&ubreve; jam mankis libera loko sur la <tld/>ega placo
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>immens</trd>,
    <trd>exorbitant</trd>,
    <trd>desmesurat</trd>,
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nep&rcaron;ehledn&yacute;</trd>,
    <trd>rozs&aacute;hl&yacute;</trd>,
    <trd>&scaron;iro&scaron;ir&yacute;</trd>,
    <trd>&scaron;ir&yacute;</trd>
  </trdgrp>
  <trd lng="fr">immense</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_o;&c_b;&c_malmol;&c_ja;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_g;&c_r;&c_o;&c_m;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_g;&c_r;&c_o;&c_m;&c_a;&c_d;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rozsiahly</trd>,
    <trd>&scaron;&iacute;ry</trd>
  </trdgrp>
</drv>

<drv mrk="vast.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="vast.0ejo.KOMUNE">
    <dif>
      <tld lit="V"/>a, pli-malpli ebena loko:
      <ekz>
        rokalta&jcirc;oj <klr>...</klr> estas alte sur la
        <tld/>ejo
        <fnt>
          <aut>-, trad. B. Ragnarsson</aut>,
          <vrk>Rakonto pri Hrafnkel</vrk>,
          <lok>en: Islandaj Pravo&ccirc;oj, 1964</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">&eacute;tendue<klr> (subst.)</klr></trd>
</drv>

<drv mrk="vast.0igi">
  <kap><ofc>*</ofc><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vast.0igi.grandigi">
    <dif>
      Fari ion <tld/>a:
      <ekz>
        duonhela kvieta lumo sin <tld/>igis tra la tuta &ccirc;ambro
        <fnt>
          <aut>L. Dilling, trad. trad. E. Hall</aut>,
          <vrk>la porcio da glacia&jcirc;o</vrk>
          <lok>en: Fundamenta Krestomatio de la lingvo Esperanto,
          1904</lok>
        </fnt>;
      </ekz>
      <ekz>
        la plej multe <tld/>igita lingvo estas la Mandarina, kiu
        <klr>...</klr> estas parolata en dek kvar a&ubreve; dek kvin el
        la dek
        na&ubreve; provincoj
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj,
        bagateloj</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>estendre</trd>,
      <trd>eixamplar</trd>,
      <trd>ampliar</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>agrandir</trd>,
      <trd>desserrer</trd>,
      <trd>d&eacute;velopper</trd>,
      <trd>r&eacute;pandre</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>t&aacute;g&iacute;t</trd>,
      <trd>b&odblac;v&iacute;t</trd>,
      <trd>sz&eacute;les&iacute;t</trd>
    </trdgrp>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_s;&c_sh;&c_y;&c_r;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_sh;&c_y;&c_r;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_a;&c_s;&c_sh;&c_i;&c_r;&c_ja;&c_t;&c_mol;</trd>
    <trdgrp lng="tr">
      <trd>geni&scedil;letmek</trd>,
      <trd>b&uuml;y&uuml;tmek</trd>
    </trdgrp>
  </snc>
  <snc mrk="vast.0igi.propagandi">
    <uzo tip="stl">ARK</uzo>
    <ref tip="dif" cel="propag.0i">propagandi</ref>
    <trdgrp lng="ca">
      <trd>divulgar</trd>,
      <trd>fer propaganda</trd>
    </trdgrp>
    <trd lng="hu">terjeszt</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>propagovat</trd>,
    <trd>roz&scaron;&iacute;&rcaron;it p&rcaron;en.
    &scaron;&iacute;&rcaron;it</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>roztiahnu&tcaron;</trd>,
    <trd>roz&scaron;&iacute;ri&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="vast.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="vast.0igxi.KOMUNE">
    <dif>
      Okupi pli grandan spacon en &ccirc;iuj direktoj:
      <ekz>
        malproksime en la fono, kie la riverbu&scirc;o <tld/>i&gcirc;as
        almare
        <fnt><bib>Kiso</bib>, <lok>la brulanta urbo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>cr&eacute;ixer</trd>,
    <trd>augmentar <klr>(intr.)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>agrandir</ind></trd>,
    <trd>s'<ind>&eacute;tendre</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>t&aacute;gul</trd>,
    <trd>b&odblac;v&uuml;l</trd>
    <trd>sz&eacute;lesedik</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_sh;&c_i;&c_r;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="vast.0igajxo">
  <kap><tld/>iga&jcirc;o</kap>
  <snc mrk="vast.0igajxo.KOMUNE">
    <dif>
      Io <tld/>igita.
    </dif>
  </snc>
  <snc mrk="vast.0igajxo.bildigo">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PIV2</bib></fnt>
    <dif>
      <klr>(de <ref tip="super" cel="bild.0igo.MAT">bildigo</ref>
      <frm am="f"><k>f</k></frm> la&ubreve; superaro <frm am="bb       A"><g>A</g></frm>
      de &gcirc;ia fonto-aro)</klr>
      Tia bildigo <frm am="g"><k>g</k></frm> de <frm am="bb       A"><g>A</g></frm> al
      la celo-aro de <frm am="f"><k>f</k></frm>, ke <frm am="g(x)=f(x)"><k>g</k>(<k>x</k>) = <k>f</k>(<k>x</k>)</frm> por &ccirc;iu elemento
      <frm am="x"><k>x</k></frm> de la fonto-aro de <frm am="f"><k>f</k></frm>:
      <ekz>
        trovu kontinuan <tld/>iga&jcirc;on de funkcio
        <frm am="sin x//sqrt x">sin<k>x</k>/&#x221A;<k>x</k></frm>
        &ccirc;e <frm>0</frm> <klr> (la&ubreve; superaro de &gcirc;ia
        fonto-aro, kiu inkluzivu punkton <frm>0</frm>, kie la funkcio
        ne estas difinita pro la divido per <frm>0</frm>)</klr>.
      </ekz>
    </dif>
    <rim>
      Por &ccirc;i tiu senco troveblas <ctl>pli<tld/>igo</ctl> en
      <fnt><bib>MatVort</bib></fnt>. Vd rimarkon sub
      <ref cel="vast.mal0igajxo.bildigo">malvastiga&jcirc;o</ref>.
    </rim>
    <trdgrp lng="de">
      <trd>Erweiterung<klr tip="ind"> (einer Abbildung)</klr></trd>,
      <trd>Fortsetzung<klr tip="ind"> (einer Abbildung)</klr></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>extension<klr tip="ind"> (of a mapping)</klr></trd>,
      <trd>continuation<klr tip="ind"> (of a mapping)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>extension<klr tip="ind"> (d'une application)</klr></trd>,
      <trd>prolongement<klr tip="ind"> (d'une application)</klr></trd>
    </trdgrp>
    <trd lng="hu">kiterjeszt&eacute;s<klr tip="ind"> (pl.
    lek&eacute;pez&eacute;s&eacute;)</klr></trd>
    <trdgrp lng="pl">
      <trd>rozszerzenie<klr tip="ind"> (funkcji)</klr></trd>,
      <trd>przed&lstroke;u&zdot;enie<klr tip="ind">
      (funkcji)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_d;&c_o;&c_l;&c_zh;&c_je;&c_n;&c_i;&c_je;<klr tip="ind">
      (&c_o;&c_t;&c_o;&c_b;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>,
      <trd>&c_r;&c_a;&c_s;&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_je;&c_n;&c_i;&c_je;<klr tip="ind">
      (&c_o;&c_t;&c_o;&c_b;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="vast.dis0igi">
  <kap>dis<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vast.dis0igi.space">
    <dif>
      Igi dise <tld/>a:
      <ekz>
        cigno, kiu dis<tld/>igas la flugilojn
        <fnt><bib>Fab1</bib>, <lok>la virineto de maro</lok></fnt>;
      </ekz>
      <ekz>
        dis<tld/>igi varmon, lumon;
      </ekz>
      <ekz>
        dis<tld/>igi malpacon<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="etend">disetendi</ref>,
      <ref cel="radi">elradii</ref>,
      <ref cel="send">elsendi</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_r;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_c;&c_ja;&c_g;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>espargir</trd>
      <trd>distendre</trd>,
      <trd>radiar</trd>      
    </trdgrp>
    <trdgrp lng="de">
      <trd>verbreiten</trd>,
      <trd>erweitern</trd>,
      <trd>ausweiten</trd>
    </trdgrp>
    <trd lng="hu">sz&eacute;tterjeszt</trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_t;&c_i;&c_r;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="vast.dis0igi.konigi">
    <dif>
      &Ccirc;ien diskonigi:
      <ekz>
        tre dis<tld/>igita opinio;
      </ekz>
      <ekz>
        ili dis<tld/>igis lian famon en tiu tuta regiono
        <fnt><bib>NT</bib>, <lok>&Mat; 9:31</lok></fnt>;
      </ekz>
      <ekz>
        la plej grava celo de nia gazeto estas &ccirc;iam zorgi pri la
        <ind>dis<tld/>igado</ind> de nia lingvo<fnt>Z</fnt>;
        <trd lng="ca">difusi&oacute;</trd>
      </ekz>
      <ekz>
        dis<tld/>i&gcirc;is lia influo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fam">disfamigi</ref>,
      <ref cel="trumpe.dis0i">distrumpeti</ref>,
      <ref cel="etend.0i">etendi</ref>,
      <ref cel="propag.0i">propagandi</ref>,
      <ref cel="sem.0i">semi</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_w;&c_s;&c_ju;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_sh;&c_y;&c_r;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>difondre</trd>,
      <trd>propagar</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>terjeszt</trd>,
      <trd>elterjeszt</trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>propagovat</trd>,
    <trd>roz&scaron;&iacute;&rcaron;it</trd>,
    <trd>roz&scaron;&iacute;&rcaron;it</trd>,
    <trd>&scaron;&iacute;&rcaron;it</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;ployer</trd>,
    <trd>propager</trd>,
    <trd>diffuser</trd>
  </trdgrp>
  <trd lng="nl">verspreiden</trd>
  <trdgrp lng="sk">
    <trd>rozprestrie&tcaron;</trd>,
    <trd>roz&scaron;&iacute;ri&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="vast.dis0igxi">
  <kap>dis<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Okupi pli kaj pli grandan spacon, atingi pli kaj pli da lokoj, da
      homoj...:
      <ekz>
        lia famo dis<tld/>i&gcirc;is en la tuta Sirio
        <fnt><bib>NT</bib>, <lok>&Mat; 4:24</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>expandir-se</trd>,
    <trd>difondre's</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>diss&eacute;miner</ind></trd>,
    <trd>se <ind>r&eacute;pandre</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>terjed</trd>,
    <trd>kiterjed</trd>,
    <trd>elterjed</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="vast.mal0a">
  <kap><ofc>*</ofc>mal<tld/>a</kap>
  <snc mrk="vast.mal0a.space">
    <dif>
      Havanta malgrandajn dimensiojn; malgrandspaca:
      <ekz>
        en la mal<tld/>a kajuto brulas lumo
        <fnt><bib>FK</bib>, <lok>4, rakontoj, nokto, (de Eube el
        Odeso)</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        butonumi iun mal<tld/>e
        <klr>(severe trakti)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        mal<tld/>e lo&gcirc;ata familio.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="strikt.0a">strikta</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_m;&c_a;&c_l;&c_y;</trd>,
      <trd>&c_n;&c_je;&c_v;&c_ja;&c_l;&c_ib;&c_k;&c_ib;</trd>,
      <trd>&c_v;&c_u;&c_z;&c_k;&c_ib;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>just</trd>,
      <trd>encofurnat</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>sz&udblac;k</trd>,
      <trd>szoros</trd>
    </trdgrp>
    <trd lng="nl">nauw</trd>
  </snc>
  <snc mrk="vast.mal0a.FIG">
    <dif>
      Havanta malgrandan amplekson:
      <ekz>
        mal<tld/>a spirito<fnt>B</fnt>;
      </ekz>
      <ekz>
        ili vivis en mal<tld/>a komuneco<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_m;&c_a;&c_l;&c_y;</trd>,
      <trd>&c_n;&c_je;&c_v;&c_ja;&c_l;&c_ib;&c_k;&c_ib;</trd>,
      <trd>&c_v;&c_u;&c_z;&c_k;&c_ib;</trd>,
      <trd>&c_c;&c_je;&c_s;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>estret</trd>,
      <trd>petit</trd>
    </trdgrp>
    <trd lng="hu">sz&udblac;k</trd>
    <trd lng="nl">klein</trd>
  </snc>
  <snc mrk="vast.mal0a.FON">
    <uzo tip="fak">FON</uzo>
    <dif>
      Prononcata kun lango pli alta kaj resonujo pli mal<tld/>a,
      ol alia sono, kun kiu oni komparas.
    </dif>
    <trd lng="be">&c_z;&c_a;&c_k;&c_r;&c_y;&c_t;&c_y;
    <klr>(&c_g;&c_u;&c_k;)</klr></trd>
    <trd lng="ca">tancat <klr>(fon&egrave;tica)</klr></trd>
    <trd lng="fr">ferm&eacute; <klr tip="ind">(phon.)</klr></trd>
    <trd lng="hu">z&aacute;rt <klr tip="ind">(hang)</klr></trd>
    <trd lng="nl">gesloten<klr tip="ind"> (klank)</klr></trd>
    <trd lng="ru">&c_z;&c_a;&c_k;&c_r;&c_y;&c_t;&c_y;&c_j; <klr tip="amb">(&c_z;&c_v;&c_u;&c_k;)</klr></trd>
  </snc>
  <trd lng="bo">&#xF55;&#xFB2;&#xF0B;&#xF54;&#xF7C;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>t&ecaron;sn&yacute;</trd>,
    <trd>&uacute;zk&yacute;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;troit</trd>,
    <trd>restreint</trd>,
    <trd>serr&eacute;</trd>,
  </trdgrp>
  <trd lng="ru">&c_u;&c_z;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>tesn&yacute;</trd>,
    <trd>&uacute;zky</trd>
  </trdgrp>
</drv>

<drv mrk="vast.mal0igajxo">
  <kap>mal<tld/>iga&jcirc;o</kap>
  <snc mrk="vast.mal0igajxo.KOMUNE">
    <dif>
      Io mal<tld/>igita.
    </dif>
    <trd lng="hu">sz&udblac;k&uuml;let</trd>
  </snc>
  <snc mrk="vast.mal0igajxo.bildigo">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PIV2</bib></fnt>
    <dif>
      <klr>(de <ref tip="super" cel="bild.0igo.MAT">bildigo</ref>
      <frm am="f"><k>f</k></frm> la&ubreve; subaro
      <frm am="bb A"><g>A</g></frm> de &gcirc;ia fonto-aro)</klr>
      Tia bildigo <frm am="g"><k>g</k></frm> de <frm am="bb       A"><g>A</g></frm> al la
      celo-aro
      de <frm am="f"><k>f</k></frm>, ke <frm am="g(x)=f(x)"><k>g</k>(<k>x</k>) = <k>f</k>(<k>x</k>)</frm> por &ccirc;iu
      <frm am="x in bb A"><k>x</k>&#x2208;<g>A</g></frm>:
      <ekz>
        la entjera adicio estas mal<tld/>iga&jcirc;o de la reela
        la&ubreve; la aro de entjeroj.
      </ekz>
    </dif>
    <ref tip="sin" cel="bild.sub0igo">subbildigo.</ref>
    <rim>
      Por &ccirc;i tiu senco troveblas <ctl>malpli<tld/>igo</ctl> en
      <fnt><bib>MatVort</bib></fnt>. La elemento <ctl>pli</ctl> ne
      aspektas
      tre utila, sed estas &gcirc;uste, ke anka&ubreve; la sufikso
      <ctl>-a&jcirc;</ctl> en nia termino, kiu teorie povus servi por
      distingi la agon disde &gcirc;ia rezulto, en praktiko estas
      balasta.
    </rim>
    <trd lng="be">&c_z;&c_v;&c_u;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
    <trd lng="de">Einschr&auml;nkung<klr tip="ind"> (einer
    Abbildung)</klr></trd>
    <trdgrp lng="en">
      <trd>restriction<klr tip="ind"> (of a mapping)</klr></trd>,
      <trd>partial <ind>mapping</ind></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>restriction<klr tip="ind"> (d'une application)</klr></trd>,
      <trd>sous-application</trd>,
      <trd>trace<klr tip="ind"> (d'une application)</klr></trd>
    </trdgrp>
    <trd lng="hu">lesz&udblac;k&iacute;t&eacute;s<klr tip="ind"> (pl.
    lek&eacute;pez&eacute;s&eacute;)</klr></trd>
    <trdgrp lng="pl">
      <trd>zacie&sacute;nienie<klr tip="ind"> (funkcji)</klr></trd>,
      <trd>obci&eogonek;cie<klr tip="ind"> (funkcji)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_u;&c_zh;&c_je;&c_n;&c_i;&c_je;<klr tip="ind">
      (&c_o;&c_t;&c_o;&c_b;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>,
      <trd>&c_o;&c_g;&c_r;&c_a;&c_n;&c_i;&c_ch;&c_je;&c_n;&c_i;&c_je;<klr tip="ind">
      (&c_o;&c_t;&c_o;&c_b;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="vast.mal0igi">
  <kap>mal<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vast.mal0igi.KOMUNE">
    <dif>
      Igi mal<tld/>a:
      <ekz>
        tiom neniu estas mal<tld/>igata en siaj rajtoj
        <fnt>
          <aut>C. Dickens, trad. L. L. Zamenhof</aut>,
          <vrk><url ref="http://www.gutenberg.org/files/24501/24501-0.txt">
          La Batalo de l' Vivo</url></vrk>,
          <lok>1891</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">resserrer</trd>
</drv>

<drv mrk="vast.mal0igxi">
  <kap>mal<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="vast.mal0igxi.KOMUNE">
    <dif>
      I&gcirc;i mal<tld/>a, space malpli etendita;
      <ref tip="vid" cel="sxrump.0i.FORM">&scirc;rumpi</ref>:
      <ekz>
        la valo pli kaj pli mal<tld/>i&gcirc;adis kaj la rivero en pli 
        mallar&gcirc;a ku&scirc;ujo pli vigle kuris
        <fnt>
          <aut>B. N&ecaron;mcov&aacute;, trad. V. Tobek kaj K.
          Proch&aacute;zka</aut>,
          <vrk><url ref="https://eo.wikisource.org/wiki/Avineto/V.">
          Avineto</url></vrk>,
          <lok>[sen dato]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">se <ind>r&eacute;duire</ind></trd>
</drv>

<drv mrk="vast.pli0igxi">
  <kap>pli<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="vast.pli0igxi.KOMUNE">
    <dif>
      I&gcirc;i pli ampleksa, pli etendi&gcirc;i:
      <ekz>
        la nigraj okuloj pli<tld/>i&gcirc;is
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxvel.0i">&scirc;veli</ref>,
      <ref cel="puf">pufi&gcirc;i</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_sh;&c_y;&c_r;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_y;&c_r;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_w;&c_s;&c_ju;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>engrandir</trd>,
    <trd>dilatar-se</trd>,
    <trd>eixamplar-se</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>&eacute;tendre</ind></trd>,
    <trd>se <ind>r&eacute;pandre</ind></trd>
  </trdgrp>
  <trd lng="hu">kit&aacute;gul</trd>
  <trd lng="nl">zich <ind>uitbreiden</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_i;&c_r;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
