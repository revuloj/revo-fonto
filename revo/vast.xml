<drv mrk="vast.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="vast.0a.space">
    <dif>
      Havanta grandajn dimensiojn, grandspaca:
      <ekz>
        <tld/>aj kaj herbori&ccirc;aj kampoj
        <fnt><bib>F</bib>, <lok>&FE; 34</lok></fnt>;
      </ekz>
      <ekz>
        iru en la <tld/>an mondon, vi sendube trovos sukceson
        <fnt><bib>ElektFab</bib>, <lok>Du fratoj</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a &ccirc;ambro
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 7a</lok></fnt>;
      </ekz>
      <ekz>
        la&ubreve;vole la&ccirc;i la &scirc;uojn pli <tld/>e a&ubreve;
        pli mal<tld/>e.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_a;&c_s;&c_t;&c_o;&c_r;&c_n;&c_y;</trd>,
      <trd>&c_sh;&c_y;&c_r;&c_o;&c_k;&c_ib;</trd>,
      <trd>&c_v;&c_ja;&c_l;&c_ib;&c_k;&c_ib;</trd>,
      <trd>&c_v;&c_ja;&c_l;&c_ib;&c_z;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>weit</trd>,
      <trd>ausgedehnt</trd>,
      <trd>ger&auml;umig</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>t&aacute;g</trd>
      <trd>t&aacute;gas</trd>,
      <trd>sz&eacute;les</trd>
    </trdgrp>
    <trd lng="nl">weids</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_o;&c_r;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_o;&c_b;&c_sh;&c_i;&c_r;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_sh;&c_i;&c_r;&c_o;&c_k;&c_i;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="vast.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Havanta grandan amplekson:
      <ekz>
        <tld/>a programo;
      </ekz>
      <ekz>
        <tld/>aj konoj;
      </ekz>
      <ekz>
        poste jam &scirc;i alpa&scirc;is al <tld/>a klarigo de sia amo
        <fnt>
          <aut>V. Devjatnin</aut>
          <vrk>Arturo</vrk>
          <lok>en: Fundamenta Krestomatio de la lingvo Esperanto,
          1904</lok>
         </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_sh;&c_y;&c_r;&c_o;&c_k;&c_ib;</trd>,
      <trd>&c_v;&c_ja;&c_l;&c_ib;&c_k;&c_ib;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>betr&auml;chtlich</trd>,
      <trd>sehr gro&szlig;</trd>,
      <trd>unermesslich</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>sz&eacute;lesk&ouml;r&udblac;</trd>,
      <trd>nagyar&aacute;ny&uacute;</trd>
    </trdgrp>
    <trd lng="nl">weids</trd>
    <trd lng="ru">&c_o;&c_b;&c_sh;&c_i;&c_r;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="vast.0a.FON">
    <uzo tip="fak">FON</uzo>
    <dif>
      Prononcata kun lango pli malalta kaj resonujo pli <tld/>a,
      ol alia sono, kun kiu oni komparas:
      <ekz>
        vokaloj estas sonoj pli <tld/>aj ol konsonantoj;
      </ekz>
      <ekz>
        en fermitaj silaboj la Esperantajn vokalojn oni ordinare
        prononcas
        <tld/>aj
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ferm.0ita">fermita</ref>
    </refgrp>
    <trd lng="be">&c_a;&c_d;&c_k;&c_r;&c_y;&c_t;&c_y;
    <klr>(&c_g;&c_u;&c_k;)</klr></trd>
    <trd lng="ca">obert <klr>(fon&egrave;tica)</klr></trd>
    <trd lng="de">breit</trd>
    <trd lng="fr">ouvert <klr tip="ind">(phon.)</klr></trd>
    <trd lng="hu">ny&iacute;lt <klr tip="ind">(hang)</klr></trd>
    <trd lng="nl">breed<klr tip="ind"> (klank)</klr></trd>
    <trd lng="ru">&c_o;&c_t;&c_k;&c_r;&c_y;&c_t;&c_y;&c_j; <klr tip="amb">(&c_z;&c_v;&c_u;&c_k;)</klr></trd>
  </snc>
  <trd lng="af">wye</trd>
  <trd lng="am">&#x1230;&#x134A;</trd>
  <trd lng="ar">&#x648;&#x627;&#x633;&#x639;</trd>
  <trd lng="az">geni&#x15F;</trd>
  <trd lng="bn">&#x9AC;&#x9CD;&#x9AF;&#x9BE;&#x9AA;&#x995;</trd>
  <trd lng="bo">&#xF42;&#xF74;&#xF0B;&#xF61;&#xF44;&#xF66;&#xF0B;&#xF54;&#xF7C;&#xF0B;</trd>
  <trd lng="bs">&#x161;irok</trd>
  <trdgrp lng="ca">
    <trd>vast</trd>,
    <trd>ampli</trd>
  </trdgrp>
  <trd lng="co">larga</trd>
  <trdgrp lng="cs">
    <trd>ob&scaron;&iacute;rn&yacute;</trd>,
    <trd>prostrann&yacute;</trd>,
    <trd>rozs&aacute;hl&yacute;</trd>,
    <trd>&scaron;irok&yacute;</trd>,
    <trd>&scaron;ir&yacute;</trd>
  </trdgrp>
  <trd lng="da">bred</trd>
  <trd lng="en">vast</trd>
  <trdgrp lng="es">
    <trd>amplio</trd>,
    <trd>vasto</trd>
  </trdgrp>
  <trd lng="et">suur</trd>
  <trd lng="eu">zabal</trd>
  <trdgrp lng="fr">
    <trd>vaste</trd>,
    <trd>l&acirc;che<klr tip="ind"> (pas serr&eacute;)</klr></trd>,
    <trd>large</trd>
  </trdgrp>
  <trd lng="fy">wiid</trd>
  <trd lng="gl">ampla</trd>
  <trd lng="gu">&#xAB5;&#xABF;&#xAB6;&#xABE;&#xAB3;</trd>
  <trd lng="hi">&#x935;&#x93F;&#x938;&#x94D;&#x924;&#x943;&#x924;</trd>
  <trd lng="hr">&#x161;irok</trd>
  <trd lng="hy">&#x56C;&#x561;&#x575;&#x576;</trd>
  <trd lng="is">brei&#xF0;ur</trd>
  <trd lng="ja">&#x5E83;&#x3044;</trd>
  <trd lng="jw">sudhut</trd>
  <trd lng="ka">&#x10E4;&#x10D0;&#x10E0;&#x10D7;&#x10DD;</trd>
  <trd lng="km">&#x1791;&#x17BC;&#x179B;&#x17B6;&#x1799;</trd>
  <trd lng="ko">&#xD3ED;</trd>
  <trd lng="lo">&#xE81;&#xEC9;&#xEA7;&#xEB2;&#xE87;</trd>
  <trd lng="ml">&#xD35;&#xD40;&#xD24;&#xD3F;&#xD2F;&#xD41;&#xD33;&#xD4D;&#xD33;</trd>
  <trd lng="mr">&#x930;&#x941;&#x902;&#x926;</trd>
  <trd lng="my">&#x1000;&#x103B;&#x101A;&#x103A;&#x101D;&#x1014;&#x103A;&#x1038;&#x101E;&#x1031;&#x102C;</trd>
  <trd lng="ne">&#x935;&#x94D;&#x92F;&#x93E;&#x92A;&#x915;</trd>
  <trd lng="ny">lonse</trd>
  <trd lng="pa">&#xA1A;&#xA4C;&#xA5C;&#xA3E;</trd>
  <trd lng="ps">&#x67E;&#x631;&#x627;&#x62E;&#x647;</trd>
  <trd lng="pt">vasto</trd>
  <trd lng="sd">&#x648;&#x68F;&#x64A;&#x646;</trd>
