<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vast</rad>/a
</kap>
<drv mrk="vast.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Havanta grandajn dimensiojn; grandspaca:
      <ekz>
        <tld/>aj kaj herbori&ccirc;aj kampoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a &ccirc;ambro, vesto<fnt>B</fnt>;
      </ekz>
      <ekz>
        la&ubreve;vole la&ccirc;i la &scirc;uojn pli <tld/>e a&ubreve; pli mal<tld/>e.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_a;&c_s;&c_t;&c_o;&c_r;&c_n;&c_y;</trd>,
      <trd>&c_sh;&c_y;&c_r;&c_o;&c_k;&c_ib;</trd>,
      <trd>&c_v;&c_ja;&c_l;&c_ib;&c_k;&c_ib;</trd>,
      <trd>&c_v;&c_ja;&c_l;&c_ib;&c_z;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>weit</trd>,
      <trd>ausgedehnt</trd>,
      <trd>ger&auml;umig</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>vaste</trd>,
      <trd>l&acirc;che<klr tip="ind"> (pas serr&eacute;)</klr></trd>,
      <trd>large</trd>
    </trdgrp>
    <trd lng="nl">weids</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_o;&c_r;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_o;&c_b;&c_sh;&c_i;&c_r;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_sh;&c_i;&c_r;&c_o;&c_k;&c_i;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Havanta grandan amplekson:
      <ekz>
        <tld/>a programo, <tld/>aj konoj;
      </ekz>
      <ekz>
        <tld/>e paroli pri io<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_sh;&c_y;&c_r;&c_o;&c_k;&c_ib;</trd>,
      <trd>&c_v;&c_ja;&c_l;&c_ib;&c_k;&c_ib;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>betr&auml;chtlich</trd>,
      <trd>sehr gro&szlig;</trd>,
      <trd>unermesslich</trd>
    </trdgrp>
    <trd lng="fr">vaste</trd>
    <trd lng="nl">weids</trd>
    <trd lng="ru">&c_o;&c_b;&c_sh;&c_i;&c_r;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc>
    <uzo tip="fak">FON</uzo>
    <dif>
      Prononcata kun lango pli malalta kaj resonujo pli <tld/>a,
      ol alia sono, kun kiu oni komparas:
      <ekz>
        vokaloj estas sonoj pli <tld/>aj ol konsonantoj;
      </ekz>
      <ekz>
        en fermitaj silaboj la Esperantajn vokalojn oni ordinare prononcas
        <tld/>aj
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fort.0a">forta</ref>,
      <ref cel="vocx.0a">vo&ccirc;a</ref>,
      <ref cel="ferm.0ita">fermita</ref>.
    </refgrp>
    <trd lng="be">&c_a;&c_d;&c_k;&c_r;&c_y;&c_t;&c_y;
    <klr>(&c_g;&c_u;&c_k;)</klr></trd>
    <trd lng="de">breit</trd>
    <trd lng="fr">ouvert <klr tip="ind">(phon.)</klr></trd>
    <trd lng="nl">breed<klr tip="ind"> (klank)</klr></trd>
    <trd lng="ru">&c_o;&c_t;&c_k;&c_r;&c_y;&c_t;&c_y;&c_j; <klr tip="amb">(&c_z;&c_v;&c_u;&c_k;)</klr></trd>
  </snc>
  <trd lng="pt">vasto</trd>
  <trd lng="en">vast</trd>
</drv>

<drv mrk="vast.0igi">
  <kap><ofc>*</ofc><tld/>igi</kap>
  <snc>
    <dif>
      Fari ion <tld/>a.
    </dif>
    <trdgrp lng="fr">
      <trd>agrandir</trd>,
      <trd>accro&icirc;tre</trd>,
      <trd>desserrer</trd>,
      <trd>d&eacute;velopper</trd>
    </trdgrp>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_s;&c_sh;&c_y;&c_r;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_sh;&c_y;&c_r;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_a;&c_s;&c_sh;&c_i;&c_r;&c_ja;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <uzo tip="stl">ARK</uzo>
    <dif>
      Propagandi.
    </dif>
  </snc>
</drv>

<drv mrk="vast.0igajxo">
  <kap><tld/>iga&jcirc;o</kap>
  <snc mrk="vast.0igajxo.KOMUNE">
    <dif>
      Io <tld/>igita.
    </dif>
  </snc>
  <snc mrk="vast.0igajxo.bildigo">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PIV2</bib></fnt>
    <dif>
      <klr>(de bildigo <frm><k>f</k></frm> la&ubreve; 
      superaro <frm><g>A</g></frm> de &gcirc;ia fonto-aro)</klr>
      Tia <ref tip="super" cel="bild.0igo.MAT">bildigo</ref> 
      <frm><k>g</k></frm> de <frm><g>A</g></frm> al la celo-aro 
      de <frm><k>f</k></frm>, ke <frm><k>g</k>(<k>x</k>) = 
      <k>f</k>(<k>x</k>)</frm> por &ccirc;iu elemento
      <frm><k>x</k></frm> de la fonto-aro de <frm><k>f</k></frm>:
      <ekz>
        trovu kontinuan <tld/>iga&jcirc;on de funkcio
        <frm>sin<k>x</k>/<k>x</k><sup>2</sup></frm> 
        &ccirc;e <frm>0</frm> <klr>(la&ubreve; superaro de &gcirc;ia 
        fonto-aro, kiu inkluzivu punkton <frm>0</frm>, kie la funkcio 
        ne estas difinita pro la divido per <frm>0</frm>)</klr>.
      </ekz>
    </dif>
    <rim>
      Por &ccirc;i tiu senco troveblas <ctl>pli<tld/>igo</ctl> en
      <fnt><bib>MatVort</bib></fnt>. Vd rimarkon sub
      <ref cel="vast.mal0igajxo.bildigo">malvastiga&jcirc;o</ref>.
    </rim>
    <trdgrp lng="de">
      <trd>Erweiterung<klr tip="ind"> (einer Abbildung)</klr></trd>,
      <trd>Fortsetzung<klr tip="ind"> (einer Abbildung)</klr></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>extension<klr tip="ind"> (of a mapping)</klr></trd>,
      <trd>continuation<klr tip="ind"> (of a mapping)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>extension<klr tip="ind"> (d'une application)</klr></trd>,
      <trd>prolongement<klr tip="ind"> (d'une application)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>rozszerzenie<klr tip="ind"> (funkcji)</klr></trd>,
      <trd>przed&lstroke;u&zdot;enie<klr tip="ind"> (funkcji)</klr></trd>
    </trdgrp> 
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_d;&c_o;&c_l;&c_zh;&c_je;&c_n;&c_i;&c_je;<klr tip="ind"> (&c_o;&c_t;&c_o;&c_b;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>,
      <trd>&c_r;&c_a;&c_s;&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_je;&c_n;&c_i;&c_je;<klr tip="ind"> (&c_o;&c_t;&c_o;&c_b;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="vast.dis0igi">
  <kap>dis<tld/>igi</kap>
  <snc>
    <dif>
      Igi dise <tld/>a:
      <ekz>
        la cigno dis<tld/>igis la flugilojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        dis<tld/>igi varmon, lumon
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="etend">disetendi</ref>,
      <ref cel="radi">elradii</ref>,
      <ref cel="send">elsendi</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_r;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_c;&c_ja;&c_g;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_t;&c_i;&c_r;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      &Ccirc;ien diskonigi:
      <ekz>
        la plej multe dis<tld/>igita lingvo en &Hcirc;inujo estas la mandarena;
      </ekz>
      <ekz>
        tre dis<tld/>igita opinio;
      </ekz>
      <ekz>
        dis<tld/>igi malpacon<fnt>Z</fnt>,
      </ekz>
      <ekz>
        famon<fnt>B</fnt>,
      </ekz>
      <ekz>
        ideon;
      </ekz>
      <ekz>
        la plej grava celo de nia gazeto estas &ccirc;iam zorgi pri la
        dis<tld/>igado de nia lingvo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        dis<tld/>i&gcirc;is lia influo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sem.0i">semi</ref>,
      <ref cel="etend.0i">etendi</ref>,
      <ref cel="trumpe.dis0i">distrumpeti</ref>,
      <ref cel="fam">disfamigi</ref>,
      <ref cel="propag.0i">propagandi</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_w;&c_s;&c_ju;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_sh;&c_y;&c_r;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>d&eacute;ployer</trd>,
    <trd>propager</trd>,
    <trd>diffuser</trd>
  </trdgrp>
  <trd lng="nl">verspreiden</trd>
</drv>

<drv mrk="vast.mal0a">
  <kap><ofc>*</ofc>mal<tld/>a</kap>
  <snc>
    <dif>
      Havanta malgrandajn dimensiojn; malgrandspaca:
      <ekz>
        mal<tld/>a kesto, pantalono;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        butonumi iun mal<tld/>e
        <klr>(severe trakti)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mal<tld/>e lo&gcirc;ata familio.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="strikt.0a">strikta</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_m;&c_a;&c_l;&c_y;</trd>,
      <trd>&c_n;&c_je;&c_v;&c_ja;&c_l;&c_ib;&c_k;&c_ib;</trd>,
      <trd>&c_v;&c_u;&c_z;&c_k;&c_ib;</trd>
    </trdgrp>
    <trd lng="nl">nauw</trd>
  </snc>
  <snc>
    <dif>
      Havanta malgrandan amplekson:
      <ekz>
        mal<tld/>a spirito<fnt>B</fnt>;
      </ekz>
      <ekz>
        ili vivis en mal<tld/>a komuneco<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiom neniu estas mal<tld/>igata en siaj rajtoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_m;&c_a;&c_l;&c_y;</trd>,
      <trd>&c_n;&c_je;&c_v;&c_ja;&c_l;&c_ib;&c_k;&c_ib;</trd>,
      <trd>&c_v;&c_u;&c_z;&c_k;&c_ib;</trd>,
      <trd>&c_c;&c_je;&c_s;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="nl">klein</trd>
  </snc>
  <snc>
    <uzo tip="fak">FON</uzo>
    <dif>
      Prononcata kun lango pli alta kaj resonujo pli mal<tld/>a,
      ol alia sono, kun kiu oni komparas.
    </dif>
    <trd lng="be">&c_z;&c_a;&c_k;&c_r;&c_y;&c_t;&c_y;
    <klr>(&c_g;&c_u;&c_k;)</klr></trd>
    <trd lng="fr">ferm&eacute; <klr tip="ind">(phon.)</klr></trd>
    <trd lng="nl">gesloten<klr tip="ind"> (klank)</klr></trd>
    <trd lng="ru">&c_z;&c_a;&c_k;&c_r;&c_y;&c_t;&c_y;&c_j; <klr tip="amb">(&c_z;&c_v;&c_u;&c_k;)</klr></trd>
  </snc>
  <trdgrp lng="fr">
    <trd>&eacute;troit</trd>,
    <trd>serr&eacute;</trd>,
    <trd>restreint</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_z;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="vast.mal0igajxo">
  <kap>mal<tld/>iga&jcirc;o</kap>
  <snc mrk="vast.mal0igajxo.KOMUNE">
    <dif>
      Io mal<tld/>igita.
    </dif>
  </snc>
  <snc mrk="vast.mal0igajxo.bildigo">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PIV2</bib></fnt>
    <dif>
      <klr>(de bildigo <frm><k>f</k></frm> la&ubreve; subaro 
      <frm><g>A</g></frm> de &gcirc;ia fonto-aro)</klr>
      Tia <ref tip="super" cel="bild.0igo.MAT">bildigo</ref> 
      <frm><k>g</k></frm> de<frm><g>A</g></frm> al la celo-aro 
      de <frm><k>f</k></frm>, ke <frm><k>g</k>(<k>x</k>) = 
      <k>f</k>(<k>x</k>)</frm> por &ccirc;iu <frm><k>x</k>&#8712;<g>A</g></frm>:
      <ekz>
        la entjera adicio estas mal<tld/>iga&jcirc;o de la reela la&ubreve;
        la aro de entjeroj.
      </ekz>
    </dif>
    <ref tip="sin" cel="bild.sub0igo">subbildigo.</ref>
    <rim>
      Por &ccirc;i tiu senco troveblas <ctl>pli<tld/>igo</ctl> en
      <fnt><bib>MatVort</bib></fnt>. La elemento <ctl>pli</ctl> ne aspektas
      tre utila, sed estas &gcirc;uste, ke anka&ubreve; la sufikso
      <ctl>-a&jcirc;</ctl> en nia termino, kiu teorie povus servi por
      distingi la agon disde &gcirc;ia rezulto, en praktiko estas balasta.
    </rim>
    <trd lng="be">&c_z;&c_v;&c_u;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
    <trd lng="de">Einschr&auml;nkung<klr tip="ind"> (einer Abbildung)</klr></trd>
    <trdgrp lng="en">
      <trd>restriction<klr tip="ind"> (of a mapping)</klr></trd>,
      <trd>partial <ind>mapping</ind></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>restriction<klr tip="ind"> (d'une application)</klr></trd>,
      <trd>sous-application</trd>,
      <trd>trace<klr tip="ind"> (d'une application)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>zacie&sacute;nienie<klr tip="ind"> (funkcji)</klr></trd>,
      <trd>obci&eogonek;cie<klr tip="ind"> (funkcji)</klr></trd></trdgrp> 
    <trdgrp lng="ru">
      <trd>&c_s;&c_u;&c_zh;&c_je;&c_n;&c_i;&c_je;<klr tip="ind"> (&c_o;&c_t;&c_o;&c_b;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>,
      <trd>&c_o;&c_g;&c_r;&c_a;&c_n;&c_i;&c_ch;&c_je;&c_n;&c_i;&c_je;<klr tip="ind"> (&c_o;&c_t;&c_o;&c_b;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="vast.pli0igxi">
  <kap>pli<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      I&gcirc;i pli ampleksa, pli etendi&gcirc;i
    </dif>
    <refgrp tip="vid">
      <ref cel="sxvel.0i">&scirc;veli</ref>,
      <ref cel="puf">pufi&gcirc;i</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_sh;&c_y;&c_r;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_y;&c_r;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_w;&c_s;&c_ju;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>&eacute;tendre</ind></trd>,
    <trd>se <ind>r&eacute;pandre</ind></trd>
  </trdgrp>
  <trd lng="nl">zich <ind>uitbreiden</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_i;&c_r;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
