<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vast</rad>/a
</kap>
<drv mrk="vast.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Havanta grandajn dimensiojn; grandspaca:
      <ekz>
        <tld/>aj kaj herbori&ccirc;aj kampoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a &ccirc;ambro, vesto<fnt>B</fnt>;
      </ekz>
      <ekz>
        la&ubreve;vole la&ccirc;i la &scirc;uojn pli <tld/>e a&ubreve; pli mal<tld/>e.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>weit</trd>,
      <trd>ausgedehnt</trd>,
      <trd>ger&auml;umig</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>vaste</trd>,
      <trd>l&acirc;che<klr tip="ind"> (pas serr&eacute;)</klr></trd>,
      <trd>large</trd>
    </trdgrp>
    <trd lng="nl">weids</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_o;&c_r;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_o;&c_b;&c_sh;&c_i;&c_r;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_sh;&c_i;&c_r;&c_o;&c_k;&c_i;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Havanta grandan amplekson:
      <ekz>
        <tld/>a programo, <tld/>aj konoj;
      </ekz>
      <ekz>
        <tld/>e paroli pri io<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>betr&auml;chtlich</trd>,
      <trd>sehr gro&szlig;</trd>,
      <trd>unermesslich</trd>
    </trdgrp>
    <trd lng="fr">vaste</trd>
    <trd lng="nl">weids</trd>
    <trd lng="ru">&c_o;&c_b;&c_sh;&c_i;&c_r;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc>
    <uzo tip="fak">FON</uzo>
    <dif>
      Prononcata kun lango pli malalta kaj resonujo pli <tld/>a,
      ol alia sono, kun kiu oni komparas:
      <ekz>
        vokaloj estas sonoj pli <tld/>aj ol konsonantoj;
      </ekz>
      <ekz>
        en fermitaj silaboj la Esperantajn vokalojn oni ordinare prononcas
        <tld/>aj
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fort.0a">forta</ref>,
      <ref cel="vocx.0a">vo&ccirc;a</ref>,
      <ref cel="ferm.0ita">fermita</ref>.
    </refgrp>
    <trd lng="de">breit</trd>
    <trd lng="fr">ouvert <klr tip="ind">(phon.)</klr></trd>
    <trd lng="nl">breed<klr tip="ind"> (klank)</klr></trd>
    <trd lng="ru">&c_o;&c_t;&c_k;&c_r;&c_y;&c_t;&c_y;&c_j; <klr tip="amb">(&c_z;&c_v;&c_u;&c_k;)</klr></trd>
  </snc>
  <trd lng="en">vast</trd>
</drv>

<drv mrk="vast.0igi">
  <kap><ofc>*</ofc><tld/>igi</kap>
  <snc>
    <dif>
      Fari ion <tld/>a.
    </dif>
    <trdgrp lng="fr">
      <trd>agrandir</trd>,
      <trd>accro&icirc;tre</trd>,
      <trd>desserrer</trd>,
      <trd>d&eacute;velopper</trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_a;&c_s;&c_sh;&c_i;&c_r;&c_ja;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <uzo tip="stl">ARK</uzo>
    <dif>
      Propagandi.
    </dif>
  </snc>
</drv>

<drv mrk="vast.dis0igi">
  <kap>dis<tld/>igi</kap>
  <snc>
    <dif>
      Igi dise <tld/>a:
      <ekz>
        la cigno dis<tld/>igis la flugilojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        dis<tld/>igi varmon, lumon
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="etend">disetendi</ref>,
      <ref cel="radi">elradii</ref>,
      <ref cel="send">elsendi</ref>.
    </refgrp>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_t;&c_i;&c_r;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      &Ccirc;ien diskonigi:
      <ekz>
        la plej multe dis<tld/>igita lingvo en &Hcirc;inujo estas la mandarena;
      </ekz>
      <ekz>
        tre dis<tld/>igita opinio;
      </ekz>
      <ekz>
        dis<tld/>igi malpacon<fnt>Z</fnt>,
      </ekz>
      <ekz>
        famon<fnt>B</fnt>,
      </ekz>
      <ekz>
        ideon;
      </ekz>
      <ekz>
        la plej grava celo de nia gazeto estas &ccirc;iam zorgi pri la
        dis<tld/>igado de nia lingvo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        dis<tld/>i&gcirc;is lia influo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sem.0i">semi</ref>,
      <ref cel="etend.0i">etendi</ref>,
      <ref cel="trumpe.dis0i">distrumpeti</ref>,
      <ref cel="fam">disfamigi</ref>,
      <ref cel="propag.0i">propagandi</ref>.
    </refgrp>
    <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>d&eacute;ployer</trd>,
    <trd>propager</trd>,
    <trd>diffuser</trd>
  </trdgrp>
  <trd lng="nl">verspreiden</trd>
</drv>

<drv mrk="vast.mal0a">
  <kap><ofc>*</ofc>mal<tld/>a</kap>
  <snc>
    <dif>
      Havanta malgrandajn dimensiojn; malgrandspaca:
      <ekz>
        mal<tld/>a kesto, pantalono;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        butonumi iun mal<tld/>e
        <klr>(severe trakti)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mal<tld/>e lo&gcirc;ata familio.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="strikt.0a">strikta</ref>.
    </refgrp>
    <trd lng="nl">nauw</trd>
  </snc>
  <snc>
    <dif>
      Havanta malgrandan amplekson:
      <ekz>
        mal<tld/>a spirito<fnt>B</fnt>;
      </ekz>
      <ekz>
        ili vivis en mal<tld/>a komuneco<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiom neniu estas mal<tld/>igata en siaj rajtoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="nl">klein</trd>
  </snc>
  <snc>
    <uzo tip="fak">FON</uzo>
    <dif>
      Prononcata kun lango pli alta kaj resonujo pli mal<tld/>a,
      ol alia sono, kun kiu oni komparas.
    </dif>
    <trd lng="fr">ferm&eacute; <klr tip="ind">(phon.)</klr></trd>
    <trd lng="nl">gesloten<klr tip="ind"> (klank)</klr></trd>
    <trd lng="ru">&c_z;&c_a;&c_k;&c_r;&c_y;&c_t;&c_y;&c_j; <klr tip="amb">(&c_z;&c_v;&c_u;&c_k;)</klr></trd>
  </snc>
  <trdgrp lng="fr">
    <trd>&eacute;troit</trd>,
    <trd>serr&eacute;</trd>,
    <trd>restreint</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_z;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="vast.mal0igajxo">
  <kap>mal<tld/>iga&jcirc;o</kap>
  <snc mrk="vast.mal0igajxo.KOMUNE">
    <dif>
      Io mal<tld/>igita.
    </dif>
  </snc>
  <snc mrk="vast.mal0igajxo.bildigo">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <klr>(de bildigo f la&ubreve; subaro <em>A</em> de &gcirc;ia
      fonto-aro)</klr>
      Tia <ref tip="super" cel="bild.0igo.MAT">bildigo</ref> g de
      <em>A</em>
      al la celo-aro de f, ke g(x)=f(x) por &ccirc;iu x&#8712;<em>A</em>:
      <ekz>
        la entjera adicio estas mal<tld/>iga&jcirc;o de la reela la&ubreve;
        la aro de entjeroj.
      </ekz>
    </dif>
    <ref tip="sin" cel="bild.sub0igo">subbildigo.</ref>
    <trd lng="fr">restriction <klr tip="ind">(d'une
    application)</klr></trd>
  </snc>
</drv>

<drv mrk="vast.pli0igxi">
  <kap>pli<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      I&gcirc;i pli ampleksa, pli etendi&gcirc;i
    </dif>
    <refgrp tip="vid">
      <ref cel="sxvel.0i">&scirc;veli</ref>,
      <ref cel="puf">pufi&gcirc;i</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>s'<ind>&eacute;tendre</ind></trd>,
    <trd>se <ind>r&eacute;pandre</ind></trd>
  </trdgrp>
  <trd lng="nl">zich <ind>uitbreiden</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_i;&c_r;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
