<drv mrk="trud.sin0i">
  <kap>sin <tld/>i</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="trud.0igxi">Trudi&gcirc;i</ref>:
      <ekz>
        tiuj ideoj pli kaj pli sin <tld/>as al la priatento de
        &ccirc;iuj popoloj;
      </ekz>
      <ekz>
        en<tld/>ante sin en tion, kion li ne vidis,
        <fnt>
          <bib>NT</bib>
          <lok>Koloseanoj 2:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        sin<tld/>anto en malpropraj aferoj
        <fnt>
          <bib>NT</bib>
          <lok>I. Petro 4:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        la demando <tld/>as sin al ni, &ccirc;u...
      </ekz>
    </dif>
    <trd lng="be">&c_n;&c_a;&c_v;&c_ja;&c_z;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
    <trd lng="fr">s'<ind>imposer</ind></trd>
    <trd lng="hu">el&odblac;t&eacute;rbe ker&uuml;l</trd>
    <trd lng="es">imponerse</trd>
    <trd lng="nl">zich <ind>opdringen</ind></trd>
    <trdgrp lng="pl">
      <trd>narzuca&cacute; si&eogonek;</trd>,
      <trd>naprzykrza&cacute; si&eogonek;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_n;&c_a;&c_v;&c_ja;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_n;&c_a;&c_v;&c_ja;&c_z;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <trd lng="de">sich <ind>aufdr&auml;ngen</ind></trd>
  <trd lng="en">to impose oneself</trd>
</drv>

