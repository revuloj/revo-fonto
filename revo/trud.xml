<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>trud</rad>/i
</kap>
<drv mrk="trud.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Devigi iun kontra&ubreve; lia volo al io malpla&ccirc;a, devige akceptigi ion al
      iu:
      <ekz>
        <tld/>i al iu tedan taskon<fnt>B</fnt>;
      </ekz>
      <ekz>
        Asirio <klr>...</klr> jam hodia&ubreve; <tld/>as al ni traktatojn
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 1a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dev.0igi">devigi</ref>,
      <ref cel="dikt.0i">dikti</ref>,
      <ref cel="neces.0a">nepra</ref>,
      <ref cel="fort.per0i">perforti</ref>,
      <ref cel="tiran">tirani</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_v;&c_ja;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">vnucovat</trd>
  <trd lng="de">aufdr&auml;ngen</trd>
  <trd lng="en">impose</trd>
  <trd lng="es">imponer</trd>
  <trd lng="fr">imposer<klr tip="amb"> (contraindre &agrave; accepter)</klr></trd>
  <trd lng="hu">k&eacute;nyszer&iacute;t</trd>  
  <trd lng="nl">opdringen</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_v;&c_ja;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_ja;&c_z;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="trud.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <uzo tip="klr">(io)</uzo>
  <snc>
    <dif>
      Esti nepre necese konsiderota a&ubreve; allasota:
      <ekz>
        anka&ubreve; aliaj pensoj <tld/>i&gcirc;is al &gcirc;i
        <fnt><bib>Fab2</bib>, <lok>malnova strata lanterno</lok></fnt>;
      </ekz>
      <ekz>
        la ekzisto de tia gazeto al<tld/>i&gcirc;as.
      </ekz>
    </dif>
    <trd lng="be">&c_n;&c_a;&c_v;&c_ja;&c_z;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
    <trd lng="es">imponerse</trd>
    <trd lng="nl">zich <ind>opdringen</ind></trd>
    <trdgrp lng="ru">
      <trd>&c_n;&c_a;&c_v;&c_ja;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_n;&c_a;&c_v;&c_ja;&c_z;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">s'<ind>imposer</ind></trd>
  <trd lng="hu">k&eacute;nyszer&uuml;l</trd>  
</drv>

<drv mrk="trud.0igxema">
  <kap><tld/>i&gcirc;ema</kap>
  <snc mrk="trud.0igxema.iu">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Inklina sin <tld/>i:
      <ekz>
        la mastrino <klr>...</klr> rigardis &scirc;in videble kiel klientinon
        pli <tld/>i&gcirc;eman kaj kompromitantan ol promesantan <klr>...</klr>
        ian profiton
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="trud.0igxema.io">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Nepre ne forpelebla, ur&gcirc;ega:
      <ekz>
        Marta vi&scirc;is per la mano siajn okulojn kvaza&ubreve; &scirc;i volus
        <klr>...</klr> forpeli de si ian <tld/>i&gcirc;eman penson
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_v;&c_ja;&c_z;&c_mol;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_d;&c_a;&c_k;&c_u;&c_ch;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_n;&c_a;&c_z;&c_o;&c_j;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_ch;&c_e;&c_p;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">aufdringlich</trd>
  <trd lng="fr">importun<klr tip="amb"> (adj.)</klr></trd>
  <trd lng="hu">tolakod&oacute;</trd>  
  <trd lng="nl">opdringerig</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_v;&c_ja;&c_z;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_a;&c_z;&c_o;&c_j;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="trud.al0i">
  <kap>al<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <ref tip="dif" cel="trud.0i">trudi</ref>
    <ekz>
      al<tld/>i sian volon<fnt>Z</fnt>;
    </ekz>
    <ekz>
      al<tld/>i siajn dezirojn al iu<fnt>Z</fnt>;
    </ekz>
    <ekz>
      mi ne penos al<tld/>i al vi tiun projekton<fnt>Z</fnt>;
    </ekz>
    <ekz>
      al<tld/>i sian superregadon al iu lando;
    </ekz>
    <ekz>
      tiu ofico al<tld/>as al li gravajn devojn;
    </ekz>
    <ekz>
      al<tld/>ita feli&ccirc;o estas ofte malfeli&ccirc;o.
    </ekz>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_v;&c_ja;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">vnucovat</trd>
  <trd lng="de">aufdr&auml;ngen</trd>
  <trd lng="en">impose</trd>
  <trd lng="es">imponer</trd>
  <trd lng="fr">imposer<klr tip="amb"> (contraindre &agrave; accepter)</klr></trd>
  <trd lng="hu">r&aacute;k&eacute;nyszer&iacute;t</trd>  
  <trd lng="nl">opdringen</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_v;&c_ja;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_ja;&c_z;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="trud.en0igxi">
  <kap>en<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      <tld lit="T"/>e sin enmiksi en fremdan aferon:
      <ekz>
        la lingvo internacia, ne en<tld/>i&gcirc;ante en la doman vivon de la
        popoloj, povas almena&ubreve; esti lingvo regna kaj societa<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_v;&c_ja;&c_z;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_u;&c_m;&c_je;&c_sh;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>imposer sa pr&eacute;sence</trd>
  </trdgrp>
  <trd lng="hu">beleavatkozik</trd>  
  <trdgrp lng="nl">
    <trd>zich <ind>opdringen</ind></trd>,
    <trd>zich <ind>bemoeien</ind></trd>
  </trdgrp>
  <trd lng="es">entrometerse</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_m;&c_je;&c_sh;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_v;&c_ja;&c_z;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="trud.en0ulo">
  <kap>en<tld/>ulo</kap>
  <snc>
    <dif>
      Tiu, kiu, tedante kaj &gcirc;enante, sin enmiksas en fremdan aferon.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_k;&c_u;&c_ch;&c_l;&c_ib;&c_v;&c_y; &c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;</trd>,
    <trd>&c_n;&c_a;&c_z;&c_o;&c_j;&c_l;&c_ib;&c_v;&c_y; &c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>g&ecirc;neur</trd>,
    <trd>importun<klr tip="amb"> (subst.)</klr></trd>,
    <trd>intrus</trd>
  </trdgrp>
  <trd lng="hu">tolakoz&oacute; alak</trd>  
  <trd lng="nl">indringer</trd>
  <trdgrp lng="es">
    <trd>entrometido</trd>,
    <trd>metido<klr> (salvadoranismo)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_v;&c_ja;&c_z;&c_ch;&c_i;&c_v;&c_y;&c_j; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>,
    <trd>&c_n;&c_a;&c_z;&c_o;&c_j;&c_l;&c_i;&c_v;&c_y;&c_j; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="trud.sin0i">
  <kap>sin <tld/>i</kap>
  <snc>
    <ref tip="dif" cel="trud.0igxi">trudi&gcirc;i</ref>
    <ekz>
      tiuj ideoj pli kaj pli sin <tld/>as al la priatento de
      &ccirc;iuj popoloj;
    </ekz>
    <ekz>
      la demando <tld/>as sin al ni, &ccirc;u...;
    </ekz>
    <trd lng="be">&c_n;&c_a;&c_v;&c_ja;&c_z;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
    <trd lng="nl">zich <ind>opdringen</ind></trd>
    <trdgrp lng="ru">
      <trd>&c_n;&c_a;&c_v;&c_ja;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_n;&c_a;&c_v;&c_ja;&c_z;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">s'<ind>imposer</ind></trd>
  <trd lng="hu">el&odblac;t&eacute;rbe ker&uuml;l</trd>  
  <trd lng="es">imponerse</trd>  
</drv>
</art>
