<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kalfatr</rad>/i
</kap>
<drv mrk="kalfat.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;topi fendojn de &scirc;ipo per stupo kaj gudro.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>kalfatern</trd>,
     <trd>abdichten</trd>
  </trdgrp>
  <trd lng="en">caulk</trd>
  <trdgrp lng="nl">
    <trd>kalfateren</trd>,
    <trd>opknappen</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_o;&c_p;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>
</drv>
</art>
