<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kalfatr</rad>/i
</kap>
<drv mrk="kalfat.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <uzo tip="fak">MAR</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;topi fendojn de &scirc;ipo 
      per <ref cel="stup.0o">stupo</ref> 
      kaj <ref cel="gudr.0o">gudro</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_k;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol; &c_ib; &c_s;&c_m;&c_a;&c_l;&c_ib;&c_c;&c_mol; <klr>(&c_k;&c_a;&c_r;&c_a;&c_b;&c_je;&c_l;&c_mol;)</klr></trd>,
    <trd>&c_z;&c_a;&c_m;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol; &c_sh;&c_ch;&c_y;&c_l;&c_ib;&c_n;&c_y; <klr>(&c_u; &c_k;&c_o;&c_r;&c_p;&c_u;&c_s;&c_je; &c_k;&c_a;&c_r;&c_a;&c_b;&c_l;&c_ja;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>kalfatern</trd>,
     <trd>abdichten</trd>
  </trdgrp>
  <trd lng="en">caulk</trd>
  <trd lng="fr">calfater</trd>
  <trdgrp lng="hu">
    <trd>dugaroz</trd>,
    <trd>kalaf&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kalfateren</trd>,
    <trd>opknappen</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_o;&c_p;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>
</drv>
</art>
