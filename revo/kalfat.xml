<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kalfat.xml,v 1.21 2018/11/25 12:10:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kalfatr</rad>/i
</kap>
<drv mrk="kalfat.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <uzo tip="fak">MAR</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="sxtop.0i.fermi">&Scirc;topi</ref>
      fendojn de &scirc;ipo per <ref cel="stup.0o">stupo</ref> 
      kaj <ref cel="gudr.0o">gudro</ref>:
      <ekz>
        stupo <klr>[&#8230;]</klr> estas interalie uzata por <tld/>i lignan &scirc;ipon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=331440"
              >Stupo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        trunkodomo konstrui<klr>[&gcirc;]</klr>is nur per granda hakilo kaj &ccirc;astran&ccirc;ilo, la
        vandoj <klr>[&#8230;]</klr> el horizontalaj arbotrunkoj <klr>[&#8230;]</klr> estis
        <tld/>itaj per tero kaj musko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=489247"
              >Trunkodomo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        viaj ri&ccirc;a&jcirc;oj, viaj komerca&jcirc;oj, viaj foira&jcirc;oj, viaj &scirc;ipanoj kaj viaj &scirc;ipestroj,
        viaj <tld/>istoj, la kondukantoj de via komercado <klr>[&#8230;]</klr> falos en la
        mezon de la maro en la tago de via pereo
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 27:27</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_k;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol; &c_ib; &c_s;&c_m;&c_a;&c_l;&c_ib;&c_c;&c_mol;
    <klr>(&c_k;&c_a;&c_r;&c_a;&c_b;&c_je;&c_l;&c_mol;)</klr></trd>,
    <trd>&c_z;&c_a;&c_m;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol; &c_sh;&c_ch;&c_y;&c_l;&c_ib;&c_n;&c_y; <klr>(&c_u; &c_k;&c_o;&c_r;&c_p;&c_u;&c_s;&c_je;
    &c_k;&c_a;&c_r;&c_a;&c_b;&c_l;&c_ja;)</klr></trd>
  </trdgrp>
  <trd lng="br">kalafeti&ntilde;</trd>
  <trd lng="ca">calafatar</trd>
  <trdgrp lng="cs">
    <trd>t&ecaron;snit</trd>,
    <trd>ucp&aacute;vat <klr>(sp&aacute;ry zejm. v lodi)</klr></trd>,
    <trd>up&ecaron;chovat</trd>,
    <trd>ut&ecaron;snit</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>kalfatern</trd>,
     <trd>abdichten</trd>
  </trdgrp>
  <trd lng="en">caulk</trd>
  <trd lng="es">calafatear</trd>
  <trd lng="fa">&a_q;&f_y;&a_r;&a_A;&a_n;&a_d;&a_w;&a_d; &f_k;&a_r;&a_d;&a_n;<klr> (&a_b;&a_d;&a_n;&a_h;&a_hamza_sure;
  &f_k;&a_sh;&a_t;&f_y;)</klr></trd>
  <trd lng="fr">calfater</trd>
  <trdgrp lng="hu">
    <trd>dugaroz</trd>,
    <trd>kalaf&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>breeuwen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kalfatrowa&cacute;</trd>,
    <trd>konopi&cacute;</trd>,
    <trd>zapycha&cacute;</trd>
  </trdgrp>   
  <trd lng="pt">calafetar</trd>
  <trd lng="ro">&icirc;nfunda</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_o;&c_p;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>zapch&aacute;va&tcaron; diery <klr>(na lodi, k&uacute;de&lcaron;ou a&nbsp;dechtom)</klr></trd>,
    <trd>zapch&aacute;va&tcaron; diery <klr>(na lodi| k&uacute;de&lcaron;ou a&nbsp;dechtom)</klr></trd>
  </trdgrp>
</drv>
</art>
