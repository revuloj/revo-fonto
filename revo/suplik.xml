<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: suplik.xml,v 1.13 2019/10/19 10:10:18 revo Exp $">
<kap>
   <rad>suplik</rad>/i <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="suplik.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <uzo tip="stl">RAR</uzo>
  <snc mrk="suplik.0i.petegi">
    <dif>
      <ref tip="dif" cel="pet.0egi">Petegi</ref>:
      <ekz>
         Anselmo lin <tld/>is <klr>[&#8230;]</klr>, ke li rigardu lian domon
         denove kiel propran
         <fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro 33a, p.
         246a</lok></fnt>;
      </ekz>
      <ekz>
        en la altaro de Eleoso &ccirc;iuj petuloj, e&ccirc; la plej hororaj krimuloj, estis
        bonven<klr>[&#8230;]</klr>aj, tial &ccirc;iuj kiuj petegis la azilon de la atenanoj
        (ekzemple Adrasto a&ubreve; la heraklidoj), rifu&gcirc;is tie por <tld/>i pardonon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=157310">Eleoso</url></lok>
        </fnt>.
      </ekz>
    </dif>  
    <ref tip="vid" cel="almoz.0i">almozi</ref> 
  </snc>
  <trd lng="bg">&c_u;&c_m;&c_o;&c_l;&c_ja;&c_v;&c_a;&c_m;</trd>
  <trd lng="ca">suplicar</trd>
  <trdgrp lng="cs">
    <trd>podat supliku</trd>,
    <trd>pon&iacute;&zcaron;en&ecaron; &zcaron;&aacute;dat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>anflehen</trd>,
    <trd>beknien</trd>
  </trdgrp>
  <trdgrp lng="fa">
   <trd>&a_t;&a_m;&a_n;&a_A; &f_k;&a_r;&a_d;&a_n;</trd>,
   <trd>&a_A;&a_s;&a_t;&a_d;&a_ayn;&a_A; &f_k;&a_r;&a_d;&a_n;</trd>,
   <trd>&a_A;&a_l;&a_t;&a_m;&a_A;&a_s; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">supplier</trd>
  <trd lng="hu">k&ouml;ny&ouml;r&ouml;g</trd>
  <trd lng="nl">smeken<klr tip="ind"> (ontr.)</klr></trd>
  <trd lng="pt">suplicar</trd>
  <trd lng="ru">&c_u;&c_m;&c_o;&c_l;&c_ja;&c_t;&c_mol;</trd>
  <trd lng="sk">pon&iacute;&zcaron;ene &zcaron;iada&tcaron;</trd>
</drv>

<drv mrk="suplik.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="suplik.0o.petego">
    <dif>
      Petego:
      <ekz>
        la krio de mia <tld/>o
        <fnt>
          <vrk><url ref="http://www.gazetoteko.com/kajeroj/k73taj.html">Tajlando: la krio de mia supliko</url></vrk>
          <lok>gazetoteko.com</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_m;&c_o;&c_l;&c_b;&c_a;</trd>
  <trd lng="ca">s&uacute;plica</trd>
  <trd lng="de">Flehen</trd>
  <trdgrp lng="fa">
     <trd>&a_t;&a_m;&a_n;&a_A;</trd>,
     <trd>&a_A;&a_s;&a_t;&a_d;&a_ayn;&a_A;</trd>,
     <trd>&a_A;&a_l;&a_t;&a_m;&a_A;&a_s;</trd>
   </trdgrp>
   <trd lng="fr">supplication</trd>
   <trd lng="hu">k&ouml;ny&ouml;rg&eacute;s</trd>  
   <trd lng="nl">smeekbede<klr tip="ind"> (ontr.)</klr></trd> 
   <trd lng="pt">s&uacute;plica</trd>
   <trd lng="ru">&c_m;&c_o;&c_l;&c_mol;&c_b;&c_a;</trd>
</drv>
</art>
