<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <rad>suplik</rad>/i <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="suplik.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <uzo tip="stl">RAR</uzo>
   <snc>
     <ref tip="dif" cel="pet.0egi">petegi</ref>
     <ekz>
       Anselmo lin <tld/>is <klr>...</klr> ke li rigardu lian domon
       denove kiel propran
       <fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro 33a, p.
       246a</lok></fnt>.
     </ekz>
   </snc>
   <trd lng="fr">supplier</trd>
   <trd lng="ru">&c_u;&c_m;&c_o;&c_l;&c_ja;&c_t;&c_mol;</trd>
</drv>

<drv mrk="suplik.0o">
   <kap><tld/>o</kap>
   <uzo tip="stl">RAR</uzo>
   <snc>
     <dif>
       Petego.
     </dif>
   </snc>
   <trd lng="fr">supplication</trd>
   <trd lng="ru">&c_m;&c_o;&c_l;&c_mol;&c_b;&c_a;</trd>
</drv>
</art>
