<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mote</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="mote.0o">
  <kap><tld/>o</kap>
  <snc mrk="mote.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Malgranda griza papilio malutila, precipe el
      familio <ref tip="super" cel="tineed.0oj.ZOO">tineedoj</ref>:
      <ekz>
        la <tld/>o, precipe nigra kun emeraldaj strioj,
        estas longa je 7 cm
        <fnt>
        <aut>R. Peres-Franko</aut>,
        <vrk><url
        ref="http://raporto.info/2005/08/05/panamo-milionoj-da-papilioj-migras-tra-panamo">
        Panamo: Amasa migrado de moteoj</url></vrk>,
        <lok>2005-08-05</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">mite</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>