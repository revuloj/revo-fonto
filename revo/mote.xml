<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mote.xml,v 1.10 2024/01/26 19:05:53 revo Exp $">
<kap>
  <rad>mote</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="mote.0o">
  <kap><tld/>o</kap>
  <snc mrk="mote.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="super" cel="papili.nokto0o">Nokta papilio</ref> negranda,
      kun mallar&gcirc;aj fran&gcirc;aj flugiloj, koloroj neokulfrapaj malhelaj, brunaj-grizaj;
      iu el familio <ref tip="super" cel="tineed.0oj.ZOO">tineedoj</ref>;
      tiujn, kies larvoj ka&ubreve;zas dama&gcirc;on al vestoj a&ubreve; nutra&jcirc;oj oni nomas
      <ke><ref tip="sub" cel="tine.0o">tineo</ref></ke>:
      <ekz>
        la <tld/>o, precipe nigra kun <klr>s</klr>meraldaj strioj,
        estas longa je 7 cm
        <fnt>
        <aut>R. Peres-Franko</aut>,
        <vrk><url ref="http://raporto.info/2005/08/05/panamo-milionoj-da-papilioj-migras-tra-panamo">
        Panamo: Amasa migrado de moteoj</url></vrk>,
        <lok>2005-08-05</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_o;&c_l;&c_mol; <klr tip="amb">(&c_k;&c_a;&c_z;&c_ju;&c_r;&c_k;&c_a;)</klr></trd>
  <trd lng="cs">mol</trd>
  <trd lng="de">Motte</trd>
  <trd lng="en">moth <klr>(Tineidae)</klr></trd>
  <trd lng="fr">mite</trd>
  <trd lng="pl">m&oacute;l</trd>
  <trd lng="ro">molie</trd>
  <trd lng="sk">mol</trd>
  <trd lng="uk">&c_m;&c_ib;&c_l;&c_mol;</trd>
  <trdgrp lng="zh">
    <trd>&#x8839; <pr>d&#xF9;</pr></trd>,
    <trd>&#x706F;&#x86FE; <pr>d&#x113;ng&#xE9;</pr></trd>,
    <trd>&#x71C8;&#x86FE; <pr>d&#x113;ng&#xE9;</pr></trd>,
    <trd>&#x98DB;&#x86FE; <pr>f&#x113;i&#xE9;</pr></trd>,
    <trd>&#x98DE;&#x86FE; <pr>f&#x113;i&#xE9;</pr></trd>
  </trdgrp>
</drv>
</art>

