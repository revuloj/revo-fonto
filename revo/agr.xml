<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: agr.xml,v 1.31 2023/10/12 16:57:43 revo Exp $">
<kap>
   <rad>agr</rad>/o
</kap>
<drv mrk="agr.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">AGR</uzo>
   <snc>
     <dif>
      Kulturata <ref tip="super" cel="kamp.0o.AGR">kampo</ref>:
      <ekz>
        la <tld/>o produktis, krom la cerealojn, anka&ubreve; legomojn
        <fnt><aut>Bardhyl Selimi</aut>: 
          <vrk><url ref="&Monato;/2001/007205.html">Voja&gcirc;impresoj, somero 2000</url></vrk>,
          <bib>Monato</bib>
          <lok>2001/01, p. 26</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>oj estas fekundigataj per fukoj
        <fnt>
          <bib>Monato</bib>
          <aut>William Simcock</aut>
          <vrk><url ref="&Monato;/2003/007873.html">Impresoj pri Portugalio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        depost senkolektivigo, la vjetnama <ind><tld/>okulturo</ind> travivis veran revolucion
        <fnt>
       <aut>Maryvonne kaj Bruno Robineau</aut>
          <vrk><url ref="&Monato;/2000/006831.php">La&ubreve; ritmo de la rizejoj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/02, p. 18</lok>
        </fnt>;
        <trd lng="de">Ackerbau</trd>
      </ekz>
      <ekz>
        la papavaj <tld/>oj en tiu lando kreskas kaj do same la kvanto de heroino
        envenanta E&ubreve;ropon
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolai Gudskov</aut>
          <vrk><url ref="&Monato;/2003/008029.html">Ordinara fa&scirc;ismo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        sen&ccirc;ese etendi&gcirc;anta dezerto atakas parton de la <ind><tld/>oareo</ind> kaj preska&ubreve; &ccirc;iujare
        okazantaj inundoj dama&gcirc;as la grenrikolton
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="&Monato;/2005/008709.php">Grenokrizo minacas</url></vrk>
        </fnt>;
        <trd lng="de">Anbaufl&auml;che</trd>
      </ekz>
      <ekz>
        &ccirc;ar la <ind><tld/>okulturisto</ind> ne uzas kemia&jcirc;ojn, kultivejoj i&gcirc;as pli fekundaj,
        man&gcirc;a&jcirc;oj estas bonkvalitaj
        <fnt>
          <bib>Monato</bib>
          <aut>Marie-Ho</aut>
          <vrk><url ref="&Monato;/2010/009979.php">La ekologiaj nutra&jcirc;oj de AMAP</url></vrk>
        </fnt>.
        <trdgrp lng="de">
          <trd>Bauer</trd>,
          <trd>Ackerbauer</trd>
        </trdgrp>
        <trd lng="nl">landbouwer</trd>
      </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_z;&c_ja;&c_m;&c_je;&c_l;&c_mol;&c_n;&c_y;
<ind>&c_n;&c_a;&c_d;&c_z;&c_je;&c_l;</ind></trd>
  <trd lng="bo">&#xF5E;&#xF72;&#xF44;&#xF0B;&#xF41;&#xF0B;</trd>
  <trd lng="ca">camp</trd>
  <trdgrp lng="cs">
    <trd>obd&ecaron;l&aacute;van&eacute; pole</trd>,
    <trd>role</trd>
  </trdgrp>
  <trd lng="de">Acker</trd>
  <trd lng="en">arable land</trd>
  <trd lng="es">agro</trd>
  <trd lng="fr"><ind>champ</ind> cultiv&eacute;</trd>
  <trdgrp lng="he">
    <trd>&#x5D0;&#x5D3;&#x5DE;&#x5D4;</trd>,
    <trd>&#x5E7;&#x5E8;&#x5E7;&#x5E2;</trd>
  </trdgrp>
  <trd lng="hu">sz&aacute;nt&oacute;f&ouml;ld</trd>
  <trd lng="it">agro<klr> (campo)</klr></trd>
  <trd lng="ja">&#x8FB2;&#x5730;  <pr>&#x306E;&#x3046;&#x3061;</pr></trd>
  <trd lng="nl">akker</trd>
  <trd lng="oc">camp</trd>
  <trdgrp lng="pt">
     <trd>agro</trd>,
     <trd>terra cultiv&aacute;vel</trd>
   </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_n;&c_i;&c_v;&c_a;</trd>,

<trd>&c_v;&c_o;&c_z;&c_d;&c_je;&c_l;&c_y;&c_v;&c_a;&c_je;&c_m;&c_o;&c_je;
<ind>&c_p;&c_o;&c_l;&c_je;</ind></trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>pole</trd>,
    <trd>pole <klr>(po&lcaron;nohospod&aacute;rske)</klr></trd>,
    <trd>p&ocirc;da <klr>(po&lcaron;nohospod&aacute;rska)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_je;&#x43C;&c_je;&#x43B;&#x44C;&#x43D;&c_a; &c_d;&c_ib;&#x43B;&#x44F;&#x43D;&#x43A;&c_a;</trd>,
    <trd>&#x43E;&c_b;&c_r;&#x43E;&c_b;&#x43B;&#x44E;&c_v;&c_a;&#x43D;&c_je; &#x43F;&#x43E;&#x43B;&c_je;</trd>,
    <trd>&c_r;&c_ib;&#x43B;&#x43B;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
