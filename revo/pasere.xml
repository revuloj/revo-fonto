<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pasere.xml,v 1.11 2018/11/25 17:10:27 revo Exp $">
<kap>
  <rad>pasered</rad>/oj
</kap>
<drv mrk="pasere.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="paser.0oformaj">paseroformaj</ref>
      birdoj (<trd lng="la">Passeridae</trd>):
      <ekz>
        &gcirc;enerale <tld/>oj kutime estas malgrandaj, diketaj brungrizaj birdoj kun
        mallongaj vostoj kaj fortikaj bekoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=108309"
              >Paseredoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_v;&c_r;&c_a;&c_b;&c_ch;&c_o;&c_v;&c_i;</trd>
  <trd lng="cs">p&ecaron;vci <klr>(&ccaron;ele&dcaron;)</klr></trd>
  <trd lng="de">Sperlinge</trd>
  <trd lng="hu">ver&eacute;bf&eacute;l&eacute;k</trd>
  <trd lng="nl">mussen en spreeuwvinken<klr tip="ind">
  (fam.)</klr></trd>
  <trd lng="pt">passer&iacute;deos</trd>
  <trd lng="ru">&c_t;&c_k;&c_a;&c_ch;&c_i;&c_k;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">spevavce</trd>
  <trd lng="sv">sparvar</trd>
</drv>
</art>
