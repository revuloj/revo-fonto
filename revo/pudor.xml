<kap>
  <rad>pudor</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pudor.0o">
  <kap><tld/>o</kap>
  <snc mrk="pudor.0o.MOR">
    <dif>
      Diskretemo pri intima&jcirc;oj, seksaferoj, sentoj:
      <ekz>
        pri unu punkto, amaj aferoj, li havis nevenkeblan <tld/>on.
        <fnt><bib>KielAkvo</bib>, <lok>1:3</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>cudnost</trd>,
    <trd>sexuální stud</trd>
  </trdgrp>
  <trd lng="de">Scham</trd>
  <trdgrp lng="fa">
    <trd>&a_H;&a_j;&a_b;</trd>,
    <trd>&a_H;&f_y;&a_A;</trd>
  </trdgrp>
  <trd lng="fr">pudeur</trd>
  <trd lng="hu">szem&eacute;rem</trd>
  <trd lng="nl">kuisheid</trd>
  <trdgrp lng="pt">
    <trd>pudor</trd>,
    <trd>pundonor</trd>,
    <trd>vergonha <klr tip="amb">sexual</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_c;&c_je;&c_l;&c_o;&c_m;&c_u;&c_d;&c_r;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_t;&c_y;&c_d;&c_l;&c_i;&c_v;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>cudnosť</trd>,
    <trd>sexuálny stud</trd>
  </trdgrp>
</drv>

<drv mrk="pudor.0a">
  <kap><tld/>a</kap>
  <snc mrk="pudor.0a.MOR">
    <dif>
      <ref tip="vid" cel="diskre.0a">Diskreta</ref> pri intima&jcirc;oj, 
      sindetenema.
    </dif>
  </snc>
  <trd lng="cs">stydlivý</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&a_H;&a_j;&a_w;&a_b;</trd>,
    <trd>&a_b;&a_A;&a_H;&f_y;&a_A;</trd>
  </trdgrp>
  <trd lng="fr">pudique</trd>
  <trd lng="hu">szem&eacute;rmes</trd>
  <trd lng="nl">kuis</trd>
  <trd lng="sk">hanblivý</trd>
</drv>
