<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tiktak</rad>/o
  <fnt>B</fnt>
</kap>
<drv mrk="tiktak.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc>
    <dif>
      Sono mem de <ref tip="vid" cel="tiktak.0o"><tld/>o</ref>.
    </dif>
  </snc>
  <trd lng="it">tic-tac</trd>
  <trd lng="de">Ticktack</trd>
  <trdgrp lng="en">
    <trd>tick</trd>,
    <trd>tick-tock</trd>
  </trdgrp>
  <trd lng="fr">tic-tac</trd>
  <trdgrp lng="pl">
    <trd>tik-tak</trd>,
    <trd>cyk-cyk</trd>,
    <trd>stuk-puk</trd>
  </trdgrp>
</drv>

<drv mrk="tiktak.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
 Alterna regula sono, kiel tiu de horlo&gcirc;o.
    </dif>
  </snc>
  <trd lng="de">Ticktack</trd>
  <trdgrp lng="fr">
    <trd>battement</trd>,
    <trd>tic-tac</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ketyeg&eacute;s</trd>,
    <trd>tiktak</trd>
  </trdgrp>
  <trd lng="nl">tiktak</trd>
  <trdgrp lng="pl">
    <trd>tykanie</trd>,
    <trd>cykanie</trd>,
    <trd>klekot</trd>,
    <trd>stukot</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>tiquetaque</trd>,
    <trd>tique-taque</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_i;&c_k;&c_a;&c_n;&c_mol;&c_je;</trd>
</drv>

<drv mrk="tiktak.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      A&ubreve;digi <tld/>on:
      <ekz>
      li tre balda&ubreve; konscii&gcirc;os pri sia atendata
      telefonfakturo, &ccirc;ar lia
      mono simple <tld/>as for<fnt><aut>Pejno Simono</aut>:
      <vrk>Bonvolu literumi!</vrk>, 
      <bib>Monato</bib>
      <lok>jaro 1996a, numero 1a, p. 22a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frap.0i.produktiSonon">frapi</ref>,
      <ref cel="perkut.0i">perkuti</ref>
    </refgrp>
  </snc>
  <trd lng="de">ticken</trd>
  <trdgrp lng="fr">
    <trd>battre<klr> (bruit)</klr></trd>,
    <trd>tambouriner</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ketyeg</trd>,
    <trd>tiktakol</trd>
  </trdgrp>
  <trd lng="nl">tikken<klr tip="ind"> (v.klok)</klr></trd>
  <trdgrp lng="pl">
    <trd>tyka&cacute;<klr tip="amb"> (np. o zegarze)</klr></trd>,
    <trd>cyka&cacute;<klr tip="amb"> (np. o bombie)</klr></trd>,
    <trd>klekota&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_i;&c_k;&c_a;&c_t;&c_mol; <klr tip="amb">(&c_o;
  &c_ch;&c_a;&c_s;&c_a;&c_h;)</klr></trd>
</drv>
</art>
