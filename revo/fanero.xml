<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fanero.xml,v 1.16 2019/04/11 06:10:16 revo Exp $">
<kap>
  <rad>fanerogam</rad>/o
</kap>
<drv mrk="fanero.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      Iu el <ref tip="prt" cel="flor.0oplant">floroplantoj</ref>:
      <ekz>
        <tld/>oj, alie ol <ref tip="ant" cel="kripto.0o">kriptogamoj</ref>
        (<ref tip="ant" cel="spor.0oplanto">sporoplantoj</ref>), 
        fekundi&gcirc;as per stamenoj kaj pistiloj kaj reprodukti&gcirc;as per semoj,
        do ili ampleksas la <ref tip="prt" cel="gimnos.0oj">gimnospermojn</ref> kaj angiospermojn
        (<ref tip="prt" cel="magnoli.0oj">magnoliojn</ref>);
      </ekz>
      <ekz>
        &Gcirc;ardeno botanika / Laboratorio de <tld lit="F"/>oj Universitato de Ne&ubreve;&scirc;atelo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=294281"
              >Akademio de natursciencoj Svisujo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <adm>
    Necesas singularigi iujn tradukojn...
  </adm>
  <trdgrp lng="ca">
    <trd>espermat&ograve;fit</trd>,
    <trd>faner&ograve;games</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>fanerogamy</trd>,
    <trd>jevnosnubn&eacute;</trd>
  </trdgrp>
  <trd lng="de">Phanerogame</trd>
  <trd lng="es">faner&oacute;gamas</trd>
  <trd lng="fr">phan&eacute;rogames</trd>
  <trd lng="hu">vir&aacute;gos n&ouml;v&eacute;nyek</trd>
  <trd lng="pt">faner&oacute;gamas</trd>
  <trd lng="ru">&c_ja;&c_v;&c_n;&c_o;&c_b;&c_r;&c_a;&c_ch;&c_n;&c_y;&c_je; &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_ja;</trd>
  <trd lng="sk">javnosnubn&eacute; rastliny</trd>
</drv>
</art>

