<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kola.xml,v 1.6 2022/03/22 15:44:11 revo Exp $">
<kap>
  <rad>kola</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="kola.0o">
  <kap><tld/>o</kap>
  <snc mrk="kola.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="dif" cel="kola.0ujo.BOT"><tld lit="K"/>ujo</ref>:
      <ekz>
        la bona efikado de la <tld/>semoj &ccirc;e la
        mara malsano dependas la&ubreve; Hamilton de la
        stimulanta efikado sur la centrojn de la
        nerva sistemo
        <fnt>
          <aut>-</aut>
          <vrk>El la mondo</vrk>
          <lok><url ref="http://anno.onb.ac.at/cgi-content/anno-plus?aid=e0a&amp;datum=1892&amp;page=24">
          La Esperantisto, 1892:2, p. 24a</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="es">cola<klr> (bot&aacute;nica)</klr></trd>
  </snc>
  <snc mrk="kola.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Sodakvo &gcirc;enerale enhavanta kafeinon
      kaj gustigita per <tld/>nukso:
      <ekz>
        tason post taso da kafo mi trinkis kiel
        kutime; Brajeno trinkis sensukeran <tld/>on
        <fnt>
          <aut>H. Becklin</aut>,
          <vrk>Al insulo ne fora, sed malsammora</vrk>,
          <lok><url ref="http://www.esperanto-chicago.org/newsletters/04_18.pdf">
          Saluton! Bulteno de la Esperanto-Societo de &Scirc;ikago, 
          2018-04</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="es">cola<klr> (bebida de refresco)</klr></trd>
    <trd lng="fr">coca<klr> (boisson)</klr></trd>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_l;&c_a; <klr>(&c_a;&c_r;&c_e;&c_h;, &c_n;&c_a;&c_p;&c_ib;&c_t;&c_a;&c_k;)</klr></trd>
  <trdgrp lng="pl">
    <trd>kola</trd>,
    <trd>cola</trd>
  </trdgrp>   
  <trd lng="ro">cola</trd>
  <trd lng="uk">&#x43A;&#x43E;&#x43B;&#x430;</trd>
</drv>

<drv mrk="kola.0ujo">
  <kap><tld/>ujo,
  <var><kap><tld/>arbo</kap></var></kap>
  <snc mrk="kola.0ujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbo el centrokcidenta Afriko,
      kun plejofte unuseksaj floroj (dioika),
      produktanta nuksojn;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref>
      el la familio
      <ref tip="malprt" cel="malvac.0oj">malvacoj</ref>
      (<trd lng="la">Cola</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_l;&c_a; <klr>(&c_d;&c_r;&c_e;&c_v;&c_a;)</klr></trd>
  <trd lng="es">cola<klr> (&aacute;rbol)</klr></trd>
  <trd lng="fr">cola</trd>
  <trd lng="pl">kola <klr>(ro&sacute;lina)</klr> </trd>     
  <trd lng="ro">cola <klr>(gen)</klr> </trd>
</drv>
</art>

