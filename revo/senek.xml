<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: senek.xml,v 1.10 2023/09/03 18:39:28 revo Exp $">
<kap>
  <rad>Senek</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="senek.0o">
  <kap><tld/>o, <var><kap><tld/>o la Juna</kap></var></kap>
  <snc mrk="senek.0o.PERS">
    <uzo tip="fak">PERS</uzo>
    <dif>
      Stoikisma filozofo romia (proksimume 4 a.K. &ndash; 65), registo,
      verkinto de leteroj, eseoj, teatra&jcirc;oj:
      <ekz>
        kiam ili ankora&ubreve; studis en la elementa lernejo, li igis ilin
        legi tekstojn de <tld/>o kaj Ovidio
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>,
        <vrk>Cent
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>;
      </ekz>
      <ekz>
        Kordovo <klr>...</klr> estis la naski&gcirc;loko interalie de la
        filozofo <tld/>o la Juna
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;Kordovo&rightquot;,
        2007-04-13</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_S;&c_e;&c_n;&c_e;&c_k;&c_a;</trd>
  <trd lng="bg">&c_L;&c_u;&c_c;&c_i;&c_j; &c_A;&c_n;&c_je;&c_j; <ind>&c_S;&c_je;&c_n;&c_je;&c_k;&c_a;</ind></trd>
  <trd lng="en">Seneca</trd>
  <trd lng="de">Seneca</trd>
  <trd lng="es">S&eacute;neca<klr tip="amb"> (Lucio Anneo S&eacute;neca)</klr></trd>
  <trd lng="fa">&a_l;&a_w;&a_s;&f_y;&a_w;&a_s; &a_A_madda;&a_n;&a_A;&a_y_hamza;&a_w;&a_s; <ind>&a_s;&a_kasra;&a_n;&a_kasra;&f_k;&a_A;</ind></trd>
  <trd lng="fr">S&eacute;n&egrave;que</trd>
  <trd lng="it">Seneca</trd>
  <trd lng="ja">&#x30BB;&#x30CD;&#x30AB;</trd>
  <trd lng="lat">Lucius Annaeus <ind>Seneca</ind></trd>
  <trd lng="pl">Seneka</trd>
  <trd lng="uk">&c_S;&c_je;&#x43D;&c_je;&#x43A;&c_a; (&c_f;&c_ib;&#x43B;&#x43E;&c_s;&#x43E;&c_f;)</trd>
</drv>
</art>

