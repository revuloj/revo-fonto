<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: paraf.xml,v 1.16 2015/07/04 08:37:41 revo Exp $">
<kap>
  <ofc>2</ofc> <rad>paraf</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="paraf.0o">
   <kap><tld/>o</kap>
   <snc mrk="paraf.0o.JUR">
     <uzo tip="fak">JUR</uzo>
     <dif>
       Mallongigita <ref tip="super"
       cel="skrib.sub0o.JUR">subskribo</ref>, kian oni uzas ekzemple
       por rapide validigi &ccirc;iujn pa&gcirc;ojn de dokumento:
       <ekz>
         mi skribas la tekston kaj via patro desegnas sube sian <tld/>on
        <fnt>
          <aut>A. Kivi, trad. H. Hall</aut>
          <vrk><url
          ref="http://www.gutenberg.org/files/28968/28968-h/28968-h.htm">
          La Botistoj</url></vrk>
          <lok>1919</lok>
        </fnt>.
       </ekz>
     </dif>
     <trd lng="hu">k&eacute;zjegy</trd>
   </snc>
   <snc mrk="paraf.0o.TIP">
     <dif>
       Distingiga streko aldonita al subskribo.
     </dif>
     <trd lng="en">paraph</trd>
     <trd lng="hu">cikornya <klr
     tip="amb">(n&eacute;val&aacute;&iacute;r&aacute;son)</klr></trd>
     <trd lng="pt">cetras</trd>     
   </snc>
  <trd lng="be">&c_r;&c_o;&c_s;&c_ch;&c_y;&c_r;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>paraf</trd>,
    <trd>parafa</trd>
  </trdgrp>
  <trd lng="de">Paraphe</trd>
  <trd lng="es">r&uacute;brica</trd>
  <trdgrp lng="fr">
     <trd>parafe</trd>,
     <trd>paraphe</trd>
   </trdgrp>
  <trd lng="nl">paraaf</trd>
  <trd lng="pt">rubrica <klr>(assinatura abreviada)</klr></trd>
  <trd lng="ru">&c_r;&c_o;&c_s;&c_ch;&c_je;&c_r;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>podpisová značka</trd>,
    <trd>šifra</trd>
  </trdgrp>
</drv>
</art>
