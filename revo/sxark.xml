<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;ark</rad>/o
</kap>

<drv mrk="sxark.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sxark.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Iu el familio <ref tip="malprt" cel="sxarke.0oj">&scirc;arkedoj</ref>
      kaj parencaj familioj.
    </dif>
    <trd lng="be">&c_a;&c_k;&c_u;&c_l;&c_a;</trd>
    <trd lng="bg">&c_a;&c_k;&c_u;&c_l;&c_a;</trd>
    <trd lng="de">Hai</trd>
    <trd lng="fr">requin</trd>
    <trd lng="hu">c&aacute;pa</trd>
    <trd lng="it">squalo</trd>
    <trd lng="nl">haai</trd>
    <trd lng="ru">&c_a;&c_k;&c_u;&c_l;&c_a;</trd>
    <trd lng="sv">haj</trd>
    <trd lng="tr">k&ouml;pekbal&inodot;&gbreve;&inodot;</trd>
  </snc>
  <snc mrk="sxark.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="sxarke.0oj">&scirc;arkedoj</ref>
      (<trd lng="la">Carcharinus</trd>).
    </dif>
    <trd lng="hu">k&eacute;k <ind>c&aacute;pa</ind></trd> 
    <trdgrp lng="it">
    <trd>squalo</trd>, 
    <trd>pescecane</trd>
    </trdgrp>
    <trd lng="nl">blauwe <ind>haai</ind></trd>
    <trd lng="ru">&c_g;&c_o;&c_l;&c_u;&c_b;&c_a;&c_ja; 
<ind>&c_a;&c_k;&c_u;&c_l;&c_a;</ind> <klr tip="amb">(&c_r;&c_o;&c_d;)</klr>
</trd>
  </snc>
  <trd lng="en">shark</trd>
  <trd lng="pt">tubar&atilde;o</trd>
</drv>

<drv mrk="sxark.balen0o">
  <kap>balen<tld/>o</kap>
  <snc mrk="sxark.balen0o.ZOO">
  <dif>
    Fi&scirc;o 12 &gcirc;is 18 metrojn longa, vivanta en tropikaj maroj, 
man&gcirc;anto 
    de planktono kaj fi&scirc;etoj
    (<trd lng="la">Rhincodon typus</trd>):
    <ekz>
      balen<tld/>oj, la plej grandaj fi&scirc;oj sur la mondo, staras sub 
risko 
      komplete estingi&gcirc;i en la venontaj jaroj 
      <fnt>
      <aut>&Gcirc;angalo</aut>,
      <vrk>Mediprotekta projekto</vrk>,
      <lok>2006-06-25</lok>
      </fnt>
    </ekz>
  </dif>
  </snc>
  <trd lng="en">whale shark</trd>
  <trd lng="fr">requin baleine</trd>
  <trd lng="hu">cetc&aacute;pa</trd>
  <trd lng="it">squalo balena</trd>
  <trd lng="nl">walvishaai</trd>
</drv>
</art>
