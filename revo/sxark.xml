<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>&scirc;ark</rad>/o
</kap>
<drv mrk="sxark.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Iu el familio <ref tip="malprt" cel="sxark.0edoj">&scirc;arkedoj</ref>
      kaj parencaj familioj.
    </dif>
    <trd lng="de">Hai</trd>
    <trd lng="fr">requin</trd>
    <trd lng="nl">haai</trd>
    <trd lng="ru">&c_a;&c_k;&c_u;&c_l;&c_a;</trd>
    <trd lng="tr">kaj&ouml;pekbal&inodot;&gbreve;&inodot;</trd>
  </snc>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="sxark.0edoj">&scirc;arkedoj</ref>
      (<trd lng="la">Carcharinus</trd>).
    </dif>
  </snc>
</drv>

</art>
