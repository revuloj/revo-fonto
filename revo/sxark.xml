<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxark.xml,v 1.46 2025/03/16 02:04:33 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;ark</rad>/o
<fnt><bib>UV</bib></fnt></kap>

<drv mrk="sxark.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sxark.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      <ref tip="lst" cel="fisx.kartilago0o" lst="voko:fi&scirc;oj">Kartilaga
      fi&scirc;o</ref> kun korpo iom spindelforma, kun hokoforma vosto kaj
      ofte triangula dorsna&gcirc;ilo:
      <ekz>
        &gcirc;i ne rompi&gcirc;is sur iu roko, neniu <tld/>o &gcirc;in englutis
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>;
      </ekz>
      <ekz>
        anka&ubreve; <tld/>o en akvario povas esti dan&gcirc;era
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2007/009350.php">&Scirc;arko-&scirc;oko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve; la sistemo de <nom>Nelson</nom>, 2006, <ref tip="prt" cel="raj.0o.KOMUNE">rajo</ref>j
        estas samrangaj kun <tld/>oj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        primitiva imperiisma kapitalismo, en kiu transprenis la ekonomiajn valorojn
        malmultaj &leftquot;<tld/>oj&#8221;, fari&gcirc;ante miliarduloj
        <fnt>
          <bib>Monato</bib>
          <aut>Zlatko Ti&scaron;ljar</aut>
          <vrk><url ref="&Monato;/2013/011135.php">Stato de la &scirc;tatoj de la iama Jugoslavio ... kaj
          la solvo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
    <trd lng="be">&c_a;&c_k;&c_u;&c_l;&c_a;</trd>
    <trd lng="bg">&c_a;&c_k;&c_u;&c_l;&c_a;</trd>
    <trd lng="ca">taur&oacute;</trd>
    <trd lng="de">Hai</trd>
    <trd lng="en">shark</trd>
    <trd lng="es">tibur&oacute;n</trd>
    <trd lng="fr">requin</trd>
    <trd lng="hu">c&aacute;pa</trd>
    <trd lng="id">hiu</trd>
    <trd lng="it">squalo</trd>
    <trdgrp lng="ja">
      <trd>&#12469;&#12513;</trd>,
      <trd>&#12501;&#12459;</trd>,
      <trd>&#24375;&#27442;&#12394;&#20154;  <pr>&#12365;&#12423;&#12358;&#12424;&#12367;&#12394;&#12376;&#12435;</pr></trd>
    </trdgrp>
    <trd lng="nl">haai</trd>
    <trd lng="pl">rekin</trd>
    <trd lng="pt">tubar&atilde;o</trd>
    <trd lng="ru">&c_a;&c_k;&c_u;&c_l;&c_a;</trd>
    <trd lng="sv">haj</trd>
    <trd lng="tl">pat&iacute;ng</trd>
    <trd lng="tr">k&ouml;pekbal&inodot;&gbreve;&inodot;</trd>
    <trd lng="uk">&c_a;&c_k;&c_u;&c_l;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#39882; <pr>sh&amacron;</pr></trd>,
    <trd>&#40104; <pr>sh&amacron;</pr></trd>,
    <trd>&#39851; <pr>ji&amacron;o</pr></trd>,
    <trd>&#40091; <pr>ji&amacron;o</pr></trd>,
    <trd>&#39882;&#39770; <pr>sh&amacron;y&uacute;</pr></trd>,
    <trd>&#40104;&#40060; <pr>sh&amacron;y&uacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxark.balen0o">
  <kap>balen<tld/>o</kap>
  <snc mrk="sxark.balen0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Plej granda vivanta <ref tip="lst" cel="fisx.0o" lst="voko:fi&scirc;oj">fi&scirc;o</ref>
      kaj <ref tip="super" cel="sxark.0o.KOMUNE"><tld/>o</ref>
      12 &gcirc;is 18 metrojn longa, vivanta en tropikaj maroj, man&gcirc;anto
      de planktono kaj fi&scirc;etoj
      (<trd lng="la">Rhincodon typus</trd>):
      <ekz>
        balen<tld/>oj, la plej grandaj fi&scirc;oj sur la mondo, staras sub
        risko komplete estingi&gcirc;i en la venontaj jaroj
        <fnt>
        <aut>&Gcirc;angalo</aut>,
        <vrk>Mediprotekta projekto</vrk>,
        <lok>2006-06-25</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/0/0a/Rhincodon_typus_fgbnms.jpg/640px-Rhincodon_typus_fgbnms.jpg" prm="PD">balen<tld/>o (Rhincodon typus)
        <fnt>
          <aut>FGBNMS/Eckert</aut>
          <lok><url ref="&WCW;/File:Rhincodon_typus_fgbnms.jpg">&WCW;/File:Rhincodon_typus_fgbnms.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_t;&c_o;&c_v;&c_a;&c_ja; <ind>&c_a;&c_k;&c_u;&c_l;&c_a;</ind></trd>
  <trd lng="ca">taur&oacute; balena</trd>
  <trd lng="de">Walhai</trd>
  <trd lng="en">whale <ind>shark</ind></trd>
  <trd lng="es">tibur&oacute;n ballena</trd>
  <trd lng="fr">requin baleine</trd>
  <trd lng="hu">cetc&aacute;pa</trd>
  <trd lng="id"><ind>hiu</ind> paus</trd>
  <trd lng="it">squalo balena</trd>
  <trd lng="ja">&#12472;&#12531;&#12505;&#12456;&#12470;&#12513;</trd>
  <trd lng="nl">walvishaai</trd>
  <trd lng="pl">rekin wielorybi</trd>
  <trdgrp lng="zh">
    <trd>&#39912;&#39882; <pr>j&imacron;ngsh&amacron;</pr></trd>,
    <trd>&#40120;&#40104; <pr>j&imacron;ngsh&amacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxark.blanka0o">
  <kap>blanka <tld/>o</kap>
  <snc mrk="sxark.blanka0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Iom granda <ref tip="lst" cel="fisx.0o" lst="voko:fi&scirc;oj">fi&scirc;o</ref>
      kaj <ref tip="super" cel="sxark.0o.KOMUNE"><tld/>o</ref>
      trovebla en &ccirc;iuj mezvarmaj maroj,
      vivanta sole a&ubreve; pare,
      &ccirc;asanto de fokoj, grandaj fi&scirc;oj, birdoj
      (<trd lng="la">Carcharodon carcharias</trd>):
      <ekz>
        oni starigis la akvarion <klr>...</klr>
        en &gcirc;i trankvile na&gcirc;is du blankaj <tld/>oj
        <fnt>
          <aut>J. Modest</aut>,
          <vrk><url ref="http://verkoj.com/verkistoj/julian-modest/dancado-kun-sarkoj/">
          Dancanta kun &scirc;arkoj</url></vrk>,
          <lok>2017</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/0/0e/Carcharodon_carcharias.jpg/640px-Carcharodon_carcharias.jpg" prm="PD">Blanka <tld/>o (Carcharodon carcharias), INsulo Guadalupe, Meksiko
        <fnt>
          <aut>Sharkdiver.com, propra verko</aut>
          <lok><url ref="&WCW;/File:Carcharodon_carcharias.jpg">&WCW;/File:Carcharodon_carcharias.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_l;&c_a;&c_ja; <ind>&c_a;&c_k;&c_u;&c_l;&c_a;</ind></trd>
  <trdgrp lng="en">
    <trd>great white <ind>shark</ind></trd>,
    <trd>white <ind>shark</ind></trd>,
    <trd>white <ind>pointer</ind> <klr>(shark)</klr></trd>,
    <trd>great <ind>white</ind> <klr>(shark)</klr></trd>
  </trdgrp>
  <trd lng="es">gran <ind>tibur&oacute;n blanco</ind></trd>
  <trd lng="fr">grand <ind>requin blanc</ind></trd>
  <trdgrp lng="pl">
    <trd>rekin bia&lstroke;y</trd>,
    <trd>&zdot;ar&lstroke;acz bia&lstroke;y</trd>
  </trdgrp>
</drv>
</art>
