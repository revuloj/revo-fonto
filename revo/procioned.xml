<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: procioned.xml,v 1.10 2023/12/22 08:30:34 revo Exp $">
<kap>
    <rad>procioned</rad>/oj <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="procioned.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="procioned.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="best.rabo0oj">rabobestoj</ref>
      (<trd lng="la">Procyonidae</trd>), al kiu apartenas
      la <ref tip="prt" cel="urs.lav0o">lavurso</ref>:
      <ekz>
         <tld/>oj <klr>[...]</klr> estas plandoirantaj, karnovoraj
         bestoj kun
         etaj &scirc;irdentoj, longa vosto
        <fnt><bib>Viki</bib>, <lok>Procionedoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_n;&c_o;&c_t;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  <trdgrp lng="en">
    <trd>Procyonidae</trd>,
    <trd>procyonids</trd>
  </trdgrp>
  <trd lng="cs">medv&iacute;dkovit&iacute;</trd>
  <trd lng="de">Kleinb&auml;ren</trd>
  <trd lng="fr">Procyonid&eacute;s</trd>
  <trd lng="pl">szopowate</trd>
  <trd lng="sk">medved&iacute;kovit&eacute;</trd>
  <trd lng="uk">&c_jeu;&#x43D;&#x43E;&c_t;&#x43E;&c_v;&c_ib;</trd>
</drv>
</art>

