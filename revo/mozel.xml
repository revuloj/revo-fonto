<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mozel.xml,v 1.16 2023/10/19 18:36:53 revo Exp $">
<kap>
  <rad>Mozel</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="mozel.M0o">
  <kap><tld/>o</kap>
  <snc mrk="mozel.M0o.GEOG">
  <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" cel="river.0o" lst="voko:riveroj">Rivero</ref> ekfluanta en <ref tip="vid" cel="franc.F0ujo">Francujo</ref>
      kaj enfluanta en <ref tip="vid" cel="rejn.R0o">Rejnon</ref> &ccirc;e
      Koblenco en <ref tip="vid" cel="german1.G0ujo">Germanujo</ref>:
      <ekz>
        &ccirc;e bordo de l' <tld/>o / en &scirc;irmo de arbust'
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o fontas &ccirc;e <nom>Col de Bussang</nom> en Vogezoj je alteco de 735 m kaj enfluas
        Rejnon post 544 km &ccirc;e <nom>Koblenz</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=50887">Mozelo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_M;&c_o;&c_z;&c_e;&c_l;&c_mol;</trd>
    <trd lng="de">Mosel</trd>
    <trd lng="fr">Moselle</trd>
    <trd lng="hu">Mosel</trd>
    <trd lng="lat">Mosella</trd>
    <trd lng="nl">Moezel</trd>
    <trd lng="pl">Mozela</trd>
    <trdgrp lng="pt">
      <trd>Mosela <klr tip="amb">(rio)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_M;&c_o;&c_z;&c_je;&c_l;&c_mol;</trd>
  </snc>
  <trd lng="ja">&#x30E2;&#x30FC;&#x30BC;&#x30EB;&#x5DDD;  <pr>&#x30E2;&#x30FC;&#x30BC;&#x30EB;&#x304B;&#x308F;</pr></trd>
  <trd lng="uk">&#x41C;&#x43E;&#x437;&#x435;&#x43B;&#x44C;</trd>
</drv>
</art>
