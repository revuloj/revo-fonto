<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Mozel</rad>/o
</kap>
<drv mrk="mozel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Rivero ekfluanta en <ref tip="vid" cel="franc.F0ujo">Francujo</ref>
      kaj enfluanta en <ref tip="vid" cel="rejn.0o">Rejnon</ref> je
      Koblenco en <ref tip="vid" cel="german1.G0ujo">Germanujo</ref>.
    </dif>
    <trd lng="de">Mosel</trd>
    <trd lng="lat">Mosella</trd>
    <trd lng="nl">Moezel</trd>
    <trd lng="pl">Mozela</trd>
    <trd lng="ru">&c_M;&c_o;&c_z;&c_je;&c_l;&c_mol;</trd>
  </snc>
</drv>
</art>
