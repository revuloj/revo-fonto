<kap>
  <rad>glacis</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="glacis.0o">
  <kap><tld/>o</kap>
  <snc mrk="glacis.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Neabrupta deklivo, natura a&ubreve; artefarita, kiu etendi&gcirc;as
      anta&ubreve; fortika&jcirc;o, paflinio ktp:
      <ekz>
        li stumblis, sed kuradis plu, supren la&ubreve; la natura <tld/>o,
        kiun la tero tie donis al la defendanto
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>  
    </dif>
  </snc>
  <trd lng="ca">glacis</trd>
  <trdgrp lng="cs">
    <trd>glacis</trd>,
    <trd>koli&scaron;t&ecaron;</trd>,
    <trd>park&aacute;n</trd>,
    <trd>p&rcaron;edhrad&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Glacis</trd>,
     <trd>Vorgel&auml;nde</trd></trdgrp>
  <trd lng="en">glacis</trd>
  <trd lng="fr">glacis<klr tip="ind"> (mil.)</klr></trd>
  <trdgrp lng="hu">
     <trd>s&iacute;kozat</trd>,
     <trd>v&aacute;rmez&odblac;</trd>,
     <trd>glacis</trd></trdgrp>
  <trd lng="nl">glacis</trd>
  <trdgrp lng="pt">
    <trd>glacis</trd>,
    <trd>esplanada</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_l;&c_a;&c_s;&c_i;&c_s;</trd>
  <trdgrp lng="sk">
    <trd>glacis</trd>,
    <trd>mierny svah <klr>(pevnostn&yacute;, z&aacute;kopov&yacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x456;&#x439;&#x441;&#x44C;&#x43A;.&#xA0;&#x491;&#x43B;&#x430;&#x441;&#x438;&#x441;</trd>,
    <trd>&#x491;&#x43B;&#x430;&#x441;&#x438;&#x441;</trd>,
    <trd>&#x441;&#x445;&#x438;&#x43B;&#x43E;&#x43A; &#x437;&#x435;&#x43C;&#x43B;&#x456;</trd>
  </trdgrp>
</drv>
