    <trd>&c_g;&c_l;&c_ja;&c_s;&c_ib;&c_s;</trd>,
    <trd>&c_l;&c_a;&c_b;&c_a;&c_v;&c_ib;&c_n;&c_a; <klr>(&c_ib;&c_n;&c_zh;.)</klr></trd>
  </trdgrp>
  <trd lng="ca">glacis</trd>
  <trdgrp lng="cs">
    <trd>glacis</trd>,
    <trd>koli&scaron;t&ecaron;</trd>,
    <trd>park&aacute;n</trd>,
    <trd>p&rcaron;edhrad&iacute;</trd>
