<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: surpri.xml,v 1.24 2023/09/04 15:14:34 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>surpriz</rad>/i
    </kap>

    <drv mrk="surpri.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="surpri.0i.KOMUNE">
        <uzo tip="klr">(io)</uzo>
        <dif>
          Mirigi per sia neatenditeco:
          <ekz>
            la nova&jcirc;o <tld/>is &ccirc;iujn<fnt>B</fnt>.
          </ekz>
        </dif>
        <trdgrp lng="be">
          <trd>&c_z;&c_mol;&c_d;&c_z;&c_ib;&c_v;&c_ib;&c_c;&c_mol; <klr tip="amb">(&c_n;&c_je;&c_ch;&c_a;&c_k;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ju;)</klr></trd>,
          <trd>&c_u;&c_r;&c_a;&c_z;&c_ib;&c_c;&c_mol; <klr tip="amb">(&c_n;&c_je;&c_ch;&c_a;&c_k;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ju;)</klr></trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_u;&c_d;&c_i;&c_v;&c_i;&c_t;&c_mol;<klr>
            &c_n;&c_je;&c_o;&c_zh;&c_i;&c_d;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;&c_ju;</klr></trd>,
          <trd>&c_p;&c_o;&c_r;&c_a;&c_z;&c_i;&c_t;&c_mol;<klr>
            &c_n;&c_je;&c_o;&c_zh;&c_i;&c_d;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;&c_ju;</klr></trd>
        </trdgrp>
      </snc>
      <snc mrk="surpri.0i.alveni">
        <dif>
          Subite kaj neatendite alveni:
          <ekz>
            la ta&ccirc;mento <tld/>is (<tld/>e atakis) la malamikon
            apud la
            arbaro<fnt>B</fnt>.
          </ekz>
        </dif>
        <trd lng="be">&c_z;&c_a;&c_s;&c_mol;&c_p;&c_je;&c_c;&c_mol;
          <klr>(&c_z;&c_mol;&c_n;&c_ja;&c_n;&c_a;&c_c;&c_k;&c_u;)</klr></trd>
        <trd lng="ru">&c_z;&c_a;&c_s;&c_t;&c_a;&c_t;&c_mol;
          &c_v;&c_r;&c_a;&c_s;&c_p;&c_l;&c_o;&c_h;</trd>
      </snc>
  <trd lng="ca">sorprendre</trd>
  <trdgrp lng="cs">
    <trd>nachytat</trd>,
    <trd>p&#x159;ekvapit</trd>,
    <trd>p&#x159;istihnout</trd>,
    <trd>zasko&#x10D;it</trd>
  </trdgrp>
  <trd lng="de">&uuml;berraschen</trd>
  <trd lng="en">surprise</trd>
  <trd lng="es">sorprender</trd>
  <trd lng="fr">surprendre</trd>
  <trd lng="hu">meglep</trd>
  <trd lng="it">sorprendere</trd>
  <trdgrp lng="ja">
    <trd>&#x4E0D;&#x610F;&#x306B;&#x8A2A;&#x308C;&#x308B;  [<ind>&#x3075;&#x3044;&#x306B;&#x304A;&#x3068;&#x305A;&#x308C;&#x308B;</ind>]</trd>,
    <trd>&#x4E0D;&#x610F;&#x6253;&#x3061;&#x3059;&#x308B;  [<ind>&#x3075;&#x3044;&#x3046;&#x3061;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x5947;&#x8972;&#x3059;&#x308B;  [<ind>&#x304D;&#x3057;&#x3085;&#x3046;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x9A5A;&#x304B;&#x3059;  [<ind>&#x304A;&#x3069;&#x308D;&#x304B;&#x3059;</ind>]</trd>,
    <trd>&#x3073;&#x3063;&#x304F;&#x308A;&#x3055;&#x305B;&#x308B;</trd>
  </trdgrp>
  <trd lng="nl">verrassen</trd>
  <trd lng="pt">surpreender</trd>
  <trd lng="sk">prekvapi&#x165;</trd>
  <trdgrp lng="uk">
    <trd>&#x437;&#x430;&#x441;&#x442;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x437;&#x430;&#x441;&#x43A;&#x43E;&#x447;&#x438;&#x442;&#x438;</trd>,
    <trd>&#x437;&#x440;&#x43E;&#x431;&#x438;&#x442;&#x438; &#x43D;&#x435;&#x441;&#x43F;&#x43E;&#x434;&#x456;&#x432;&#x430;&#x43D;&#x43A;&#x443;</trd>,
    <trd>&#x434;&#x438;&#x432;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x440;&#x430;&#x436;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

    <drv mrk="surpri.0a">
      <kap><tld/>a</kap>
      <snc>
        <dif>
          Tute neatendita, <tld/>anta:
          <ekz>
            tute <tld/>a nomo inter la kandidatoj estas Fran&ccedil;ois
            Simonnet
            el Francio
            <fnt><aut>Libera Folio</aut><vrk>Rekordaj 16 kandidatoj por
            6
                lokoj</vrk>
              <bib>LOdE</bib><lok>jaro 2004a, numero 4a</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="ca">sorprenent</trd>
      <trd lng="en">surprise</trd>
      <trd lng="es">sorprendente</trd>
      <trd lng="fr">surprenant</trd>
      <trd lng="it">sorprendente</trd>
      <trdgrp lng="ja">
    <trd>&#x4E0D;&#x610F;&#x306E;  [<ind>&#x3075;&#x3044;&#x306E;</ind>]</trd>,
    <trd>&#x5947;&#x8972;&#x306E;  [<ind>&#x304D;&#x3057;&#x3085;&#x3046;&#x306E;</ind>]</trd>,
    <trd>&#x601D;&#x3044;&#x304C;&#x3051;&#x306A;&#x3044;  [<ind>&#x304A;&#x3082;&#x3044;&#x304C;&#x3051;&#x306A;&#x3044;</ind>]</trd>,
    <trd>&#x9A5A;&#x304F;&#x3079;&#x304D;  [<ind>&#x304A;&#x3069;&#x308D;&#x304F;&#x3079;&#x304D;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">verrassend</trd>
      <trd lng="pt">surpreendente</trd>
      <trdgrp lng="ru">
        <trd>&c_u;&c_d;&c_i;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;<klr>
            &c_n;&c_je;&c_o;&c_zh;&c_i;&c_d;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;&c_ju;</klr></trd>,
        <trd>&c_n;&c_je;&c_o;&c_zh;&c_i;&c_d;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
      </trdgrp>
      <trdgrp lng="uk">
    <trd>&#x43D;&#x435;&#x441;&#x43F;&#x43E;&#x434;&#x456;&#x432;&#x430;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x432;&#x440;&#x430;&#x436;&#x430;&#x44E;&#x447;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

    <drv mrk="surpri.0igxi">
      <kap><tld/>i&gcirc;i</kap>
      <snc>
        <dif>
          I&gcirc;i <tld/>ita:
          <ekz>
            kiam mi a&ubreve;dis la pa&scirc;ojn, mi <tld/>i&gcirc;is,
            sed vi
            tuj aperis
            <fnt><aut>Machado de Assis, trad. Paulo Viana</aut>
              <vrk>La divenistino kaj aliaj rakontoj</vrk>
              <lok>Oportuno, jaro 2005a, p. 30a</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="ca">sorprendre's</trd>
      <trd lng="en">be surprised</trd>
      <trd lng="es">sorprenderse</trd>
      <trd lng="fr">&ecirc;tre <ind>surpris</ind></trd>
      <trd lng="it">sorprendersi</trd>
      <trdgrp lng="ja">
    <trd>&#x9A5A;&#x304F;  [<ind>&#x304A;&#x3069;&#x308D;&#x304F;</ind>]</trd>,
    <trd>&#x3073;&#x3063;&#x304F;&#x308A;&#x3059;&#x308B;</trd>,
    <trd>&#x9762;&#x98DF;&#x3089;&#x3046;  [<ind>&#x3081;&#x3093;&#x304F;&#x3089;&#x3046;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">verschieten</trd>
      <trd lng="pt">surpreender-se</trd>
      <trdgrp lng="ru">
        <trd>&c_u;&c_d;&c_i;&c_v;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
        <trd>&c_p;&c_o;&c_r;&c_a;&c_z;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
      </trdgrp>
    </drv>

    <drv mrk="surpri.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc>
        <dif>
          Ago <tld/>i; io, kio <tld/>as<sncref ref="surpri.0i.KOMUNE"/>:
          <ekz>
            mi eksaltis de <tld/>o
            <fnt><bib>F</bib>, <lok>&FE;40.</lok></fnt>;
          </ekz><ekz>
            tri el liaj noveloj fini&gcirc;as per <tld/>o por la leganto
            <fnt><aut>Edmund Grimley Evans</aut><vrk>Denove kvaropas
                (Fern&aacute;ndez, Camacho, Neves, Dek; Ekstremoj)</vrk>
              <bib>Monato</bib><lok>jaro 1998a, numero 2a, p.
              15</lok></fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="ca">sorpresa</trd>
  <trd lng="cs">p&#x159;ekvapen&#xED;</trd>
  <trd lng="en">surprise</trd>
  <trd lng="es">sorpresa</trd>
  <trd lng="fr">surprise</trd>
  <trd lng="it">sorpresa</trd>
  <trdgrp lng="ja">
    <trd>&#x4E0D;&#x610F;&#x6253;&#x3061;  [<ind>&#x3075;&#x3044;&#x3046;&#x3061;</ind>]</trd>,
    <trd>&#x5947;&#x8972;  [<ind>&#x304D;&#x3057;&#x3085;&#x3046;</ind>]</trd>,
    <trd>&#x4E0D;&#x610F;&#x306E;&#x51FA;&#x6765;&#x4E8B;  [<ind>&#x3075;&#x3044;&#x306E;&#x3067;&#x304D;&#x3054;&#x3068;</ind>]</trd>,
    <trd>&#x9A5A;&#x304D;  [<ind>&#x304A;&#x3069;&#x308D;&#x304D;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
        <trd>verrassing</trd>,
        <trd>surprise</trd>
      </trdgrp>
  <trd lng="pt">surpresa<klr tip="ind"> (a&ccedil;&atilde;o de
          surpreender)</klr></trd>
  <trdgrp lng="ru">
        <trd>&c_n;&c_je;&c_o;&c_zh;&c_i;&c_d;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
        <trd>&c_s;&c_ju;&c_r;&c_p;&c_r;&c_i;&c_z;</trd>
      </trdgrp>
  <trd lng="sk">prekvapenie</trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x44E;&#x440;&#x43F;&#x440;&#x438;&#x437;</trd>,
    <trd>&#x43D;&#x435;&#x441;&#x43F;&#x43E;&#x434;&#x456;&#x432;&#x430;&#x43D;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>
