    <trd>&#x4E0D;&#x610F;&#x306E;  <pr>&#x3075;&#x3044;&#x306E;</pr></trd>,
    <trd>&#x5947;&#x8972;&#x306E;  <pr>&#x304D;&#x3057;&#x3085;&#x3046;&#x306E;</pr></trd>,
    <trd>&#x601D;&#x3044;&#x304C;&#x3051;&#x306A;&#x3044;  <pr>&#x304A;&#x3082;&#x3044;&#x304C;&#x3051;&#x306A;&#x3044;</pr></trd>,
    <trd>&#x9A5A;&#x304F;&#x3079;&#x304D;  <pr>&#x304A;&#x3069;&#x308D;&#x304F;&#x3079;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="nl">verrassend</trd>
