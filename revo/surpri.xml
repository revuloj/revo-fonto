  <trdgrp lng="zh">
    <trd>&#x60CA;&#x559C; <pr>j&#x12B;ngx&#x1D0;</pr></trd>,
    <trd>&#x9A5A;&#x559C; <pr>j&#x12B;ngx&#x1D0;</pr></trd>
  </trdgrp>
</drv>
