        <trd>surprise</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>niespodzianka</trd>,
    <trd>zaskoczenie</trd>
  </trdgrp>
  <trd lng="pt">surpresa<klr tip="ind"> (a&ccedil;&atilde;o de
