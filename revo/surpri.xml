    <drv mrk="surpri.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="surpri.0i.KOMUNE">
        <uzo tip="klr">(io)</uzo>
        <dif>
          Mirigi per sia neatenditeco:
          <ekz>
            la nova&jcirc;o <tld/>is &ccirc;iujn<fnt>B</fnt>.
          </ekz>
        </dif>
        <trdgrp lng="be">
          <trd>&c_z;&c_mol;&c_d;&c_z;&c_ib;&c_v;&c_ib;&c_c;&c_mol; <klr tip="amb">(&c_n;&c_je;&c_ch;&c_a;&c_k;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ju;)</klr></trd>,
          <trd>&c_u;&c_r;&c_a;&c_z;&c_ib;&c_c;&c_mol; <klr tip="amb">(&c_n;&c_je;&c_ch;&c_a;&c_k;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ju;)</klr></trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_u;&c_d;&c_i;&c_v;&c_i;&c_t;&c_mol;<klr>
            &c_n;&c_je;&c_o;&c_zh;&c_i;&c_d;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;&c_ju;</klr></trd>,
          <trd>&c_p;&c_o;&c_r;&c_a;&c_z;&c_i;&c_t;&c_mol;<klr>
            &c_n;&c_je;&c_o;&c_zh;&c_i;&c_d;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;&c_ju;</klr></trd>
        </trdgrp>
      </snc>
      <snc mrk="surpri.0i.alveni">
        <dif>
          Subite kaj neatendite alveni:
          <ekz>
            la ta&ccirc;mento <tld/>is (<tld/>e atakis) la malamikon
            apud la
            arbaro<fnt>B</fnt>.
          </ekz>
        </dif>
        <trd lng="be">&c_z;&c_a;&c_s;&c_mol;&c_p;&c_je;&c_c;&c_mol;
          <klr>(&c_z;&c_mol;&c_n;&c_ja;&c_n;&c_a;&c_c;&c_k;&c_u;)</klr></trd>
        <trd lng="ru">&c_z;&c_a;&c_s;&c_t;&c_a;&c_t;&c_mol;
          &c_v;&c_r;&c_a;&c_s;&c_p;&c_l;&c_o;&c_h;</trd>
      </snc>
  <trd lng="ca">sorprendre</trd>
  <trdgrp lng="cs">
    <trd>nachytat</trd>,
    <trd>p&rcaron;ekvapit</trd>,
    <trd>p&rcaron;istihnout</trd>,
    <trd>zasko&#x10D;it</trd>
  </trdgrp>
  <trd lng="de">&uuml;berraschen</trd>
  <trd lng="en">surprise</trd>
  <trd lng="es">sorprender</trd>
  <trd lng="fr">surprendre</trd>
  <trd lng="hu">meglep</trd>
  <trd lng="it">sorprendere</trd>
  <trdgrp lng="ja">
    <trd>&#x4E0D;&#x610F;&#x306B;&#x8A2A;&#x308C;&#x308B; 
    <pr>&#x3075;&#x3044;&#x306B;&#x304A;&#x3068;&#x305A;&#x308C;&#x308B;</pr></trd>,
    <trd>&#x4E0D;&#x610F;&#x6253;&#x3061;&#x3059;&#x308B;  <pr>&#x3075;&#x3044;&#x3046;&#x3061;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5947;&#x8972;&#x3059;&#x308B;  <pr>&#x304D;&#x3057;&#x3085;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x9A5A;&#x304B;&#x3059;  <pr>&#x304A;&#x3069;&#x308D;&#x304B;&#x3059;</pr></trd>,
    <trd>&#x3073;&#x3063;&#x304F;&#x308A;&#x3055;&#x305B;&#x308B;</trd>
  </trdgrp>
  <trd lng="nl">verrassen</trd>
  <trdgrp lng="pl">
    <trd>niespodziank&eogonek; robi&cacute;</trd>,
    <trd>zadziwia&cacute;</trd>,
    <trd>zaskakiwa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">surpreender</trd>
  <trd lng="sk">prekvapi&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_s;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_s;&#x43A;&#x43E;&c_ch;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_r;&#x43E;&c_b;&c_i;&c_t;&c_i; &#x43D;&c_je;&c_s;&#x43F;&#x43E;&c_d;&c_ib;&c_v;&c_a;&#x43D;&#x43A;&c_u;</trd>,
    <trd>&c_d;&c_i;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4F7F;&#x611F;&#x5230;&#x610F;&#x5916; <pr>sh&#x1D0;g&#x1CE;nd&#xE0;oy&#xEC;w&#xE0;i</pr></trd>,
    <trd>&#x610F;&#x5916;&#x88AD;&#x51FB; <pr>y&#xEC;w&#xE0;ix&#xED;j&#x12B;</pr></trd>,
    <trd>&#x610F;&#x5916;&#x8972;&#x64CA; <pr>y&#xEC;w&#xE0;ix&#xED;j&#x12B;</pr></trd>,
    <trd>&#x60CA;&#x8BB6; <pr>j&#x12B;ngy&#xE0;</pr></trd>,
    <trd>&#x9A5A;&#x8A1D; <pr>j&#x12B;ngy&#xE0;</pr></trd>,
    <trd>&#x63DC; <pr>y&#x1CE;n</pr></trd>
  </trdgrp>
</drv>

    <drv mrk="surpri.0a">
      <kap><tld/>a</kap>
      <snc>
        <dif>
          Tute neatendita, <tld/>anta:
          <ekz>
            tute <tld/>a nomo inter la kandidatoj estas Fran&ccedil;ois
            Simonnet
            el Francio
            <fnt><aut>Libera Folio</aut><vrk>Rekordaj 16 kandidatoj por
            6
                lokoj</vrk>
              <bib>LOdE</bib><lok>jaro 2004a, numero 4a</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="ca">sorprenent</trd>
      <trd lng="en">surprise</trd>
      <trd lng="es">sorprendente</trd>
      <trd lng="fr">surprenant</trd>
      <trd lng="it">sorprendente</trd>
