<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: surpri.xml,v 1.26 2023/11/28 01:36:39 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>surpriz</rad>/i
    </kap>

    <drv mrk="surpri.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="surpri.0i.KOMUNE">
        <uzo tip="klr">(io)</uzo>
        <dif>
          Mirigi per sia neatenditeco:
          <ekz>
            la nova&jcirc;o <tld/>is &ccirc;iujn<fnt>B</fnt>.
          </ekz>
        </dif>
        <trdgrp lng="be">
          <trd>&c_z;&c_mol;&c_d;&c_z;&c_ib;&c_v;&c_ib;&c_c;&c_mol; <klr tip="amb">(&c_n;&c_je;&c_ch;&c_a;&c_k;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ju;)</klr></trd>,
          <trd>&c_u;&c_r;&c_a;&c_z;&c_ib;&c_c;&c_mol; <klr tip="amb">(&c_n;&c_je;&c_ch;&c_a;&c_k;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ju;)</klr></trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_u;&c_d;&c_i;&c_v;&c_i;&c_t;&c_mol;<klr>
            &c_n;&c_je;&c_o;&c_zh;&c_i;&c_d;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;&c_ju;</klr></trd>,
          <trd>&c_p;&c_o;&c_r;&c_a;&c_z;&c_i;&c_t;&c_mol;<klr>
            &c_n;&c_je;&c_o;&c_zh;&c_i;&c_d;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;&c_ju;</klr></trd>
        </trdgrp>
      </snc>
      <snc mrk="surpri.0i.alveni">
        <dif>
          Subite kaj neatendite alveni:
          <ekz>
            la ta&ccirc;mento <tld/>is (<tld/>e atakis) la malamikon
            apud la
            arbaro<fnt>B</fnt>.
          </ekz>
        </dif>
        <trd lng="be">&c_z;&c_a;&c_s;&c_mol;&c_p;&c_je;&c_c;&c_mol;
          <klr>(&c_z;&c_mol;&c_n;&c_ja;&c_n;&c_a;&c_c;&c_k;&c_u;)</klr></trd>
        <trd lng="ru">&c_z;&c_a;&c_s;&c_t;&c_a;&c_t;&c_mol;
          &c_v;&c_r;&c_a;&c_s;&c_p;&c_l;&c_o;&c_h;</trd>
      </snc>
  <trd lng="ca">sorprendre</trd>
  <trdgrp lng="cs">
    <trd>nachytat</trd>,
    <trd>p&rcaron;ekvapit</trd>,
    <trd>p&rcaron;istihnout</trd>,
    <trd>zasko&#x10D;it</trd>
  </trdgrp>
  <trd lng="de">&uuml;berraschen</trd>
  <trd lng="en">surprise</trd>
  <trd lng="es">sorprender</trd>
  <trd lng="fr">surprendre</trd>
  <trd lng="hu">meglep</trd>
  <trd lng="it">sorprendere</trd>
  <trdgrp lng="ja">
    <trd>&#x4E0D;&#x610F;&#x306B;&#x8A2A;&#x308C;&#x308B; 
    <pr>&#12405;&#12356;&#x306B;&#x304A;&#12392;&#x305A;&#x308C;&#x308B;</pr></trd>,
    <trd>&#x4E0D;&#x610F;&#25171;&#12385;&#12377;&#x308B;  <pr>&#12405;&#12356;&#12358;&#12385;&#12377;&#x308B;</pr></trd>,
    <trd>&#22855;&#35186;&#12377;&#x308B;  <pr>&#x304D;&#12375;&#12421;&#12358;&#12377;&#x308B;</pr></trd>,
    <trd>&#x9A5A;&#x304B;&#12377;  <pr>&#x304A;&#12393;&#x308D;&#x304B;&#12377;</pr></trd>,
    <trd>&#12403;&#12387;&#x304F;&#x308A;&#12373;&#x305B;&#x308B;</trd>
  </trdgrp>
  <trd lng="nl">verrassen</trd>
  <trdgrp lng="pl">
    <trd>niespodziank&eogonek; robi&cacute;</trd>,
    <trd>zadziwia&cacute;</trd>,
    <trd>zaskakiwa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">surpreender</trd>
  <trd lng="sk">prekvapi&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_s;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_s;&#x43A;&#x43E;&c_ch;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_r;&#x43E;&c_b;&c_i;&c_t;&c_i; &#x43D;&c_je;&c_s;&#x43F;&#x43E;&c_d;&c_ib;&c_v;&c_a;&#x43D;&#x43A;&c_u;</trd>,
    <trd>&c_d;&c_i;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

    <drv mrk="surpri.0a">
      <kap><tld/>a</kap>
      <snc>
        <dif>
          Tute neatendita, <tld/>anta:
          <ekz>
            tute <tld/>a nomo inter la kandidatoj estas Fran&ccedil;ois
            Simonnet
            el Francio
            <fnt><aut>Libera Folio</aut><vrk>Rekordaj 16 kandidatoj por
            6
                lokoj</vrk>
              <bib>LOdE</bib><lok>jaro 2004a, numero 4a</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="ca">sorprenent</trd>
      <trd lng="en">surprise</trd>
      <trd lng="es">sorprendente</trd>
      <trd lng="fr">surprenant</trd>
      <trd lng="it">sorprendente</trd>
      <trdgrp lng="ja">
    <trd>&#x4E0D;&#x610F;&#x306E;  <pr>&#12405;&#12356;&#x306E;</pr></trd>,
    <trd>&#22855;&#35186;&#x306E;  <pr>&#x304D;&#12375;&#12421;&#12358;&#x306E;</pr></trd>,
    <trd>&#x601D;&#12356;&#x304C;&#12369;&#x306A;&#12356;  <pr>&#x304A;&#12418;&#12356;&#x304C;&#12369;&#x306A;&#12356;</pr></trd>,
    <trd>&#x9A5A;&#x304F;&#12409;&#x304D;  <pr>&#x304A;&#12393;&#x308D;&#x304F;&#12409;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="nl">verrassend</trd>
  <trdgrp lng="pl">
    <trd>niespodziewany</trd>,
    <trd>niespodziany</trd>
  </trdgrp>
  <trd lng="pt">surpreendente</trd>
  <trdgrp lng="ru">
     <trd>&c_u;&c_d;&c_i;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;<klr> &c_n;&c_je;&c_o;&c_zh;&c_i;&c_d;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;&c_ju;</klr></trd>,
     <trd>&c_n;&c_je;&c_o;&c_zh;&c_i;&c_d;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_je;&c_s;&#x43F;&#x43E;&c_d;&c_ib;&c_v;&c_a;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_r;&c_a;&c_zh;&c_a;&#x44E;&c_ch;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

    <drv mrk="surpri.0igxi">
      <kap><tld/>i&gcirc;i</kap>
      <snc>
        <dif>
          I&gcirc;i <tld/>ita:
          <ekz>
            kiam mi a&ubreve;dis la pa&scirc;ojn, mi <tld/>i&gcirc;is,
            sed vi
            tuj aperis
            <fnt><aut>Machado de Assis, trad. Paulo Viana</aut>
              <vrk>La divenistino kaj aliaj rakontoj</vrk>
              <lok>Oportuno, jaro 2005a, p. 30a</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="ca">sorprendre's</trd>
      <trd lng="en">be surprised</trd>
      <trd lng="es">sorprenderse</trd>
      <trd lng="fr">&ecirc;tre <ind>surpris</ind></trd>
      <trd lng="it">sorprendersi</trd>
      <trdgrp lng="ja">
    <trd>&#x9A5A;&#x304F;  <pr>&#x304A;&#12393;&#x308D;&#x304F;</pr></trd>,
    <trd>&#12403;&#12387;&#x304F;&#x308A;&#12377;&#x308B;</trd>,
    <trd>&#38754;&#x98DF;&#12425;&#12358;  <pr>&#12417;&#12435;&#x304F;&#12425;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">verschieten</trd>
  <trd lng="pl">zaskoczy&cacute; si&eogonek;</trd>
  <trd lng="pt">surpreender-se</trd>
  <trdgrp lng="ru">
     <trd>&c_u;&c_d;&c_i;&c_v;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
     <trd>&c_p;&c_o;&c_r;&c_a;&c_z;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  </drv>

    <drv mrk="surpri.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc>
        <dif>
          Ago <tld/>i; io, kio <tld/>as<sncref ref="surpri.0i.KOMUNE"/>:
          <ekz>
            mi eksaltis de <tld/>o
            <fnt><bib>F</bib>, <lok>&FE;40.</lok></fnt>;
          </ekz><ekz>
            tri el liaj noveloj fini&gcirc;as per <tld/>o por la leganto
            <fnt><aut>Edmund Grimley Evans</aut><vrk>Denove kvaropas
                (Fern&aacute;ndez, Camacho, Neves, Dek; Ekstremoj)</vrk>
              <bib>Monato</bib><lok>jaro 1998a, numero 2a, p.
              15</lok></fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="ca">sorpresa</trd>
  <trd lng="cs">p&rcaron;ekvapen&#xED;</trd>
  <trd lng="en">surprise</trd>
  <trd lng="es">sorpresa</trd>
  <trd lng="fr">surprise</trd>
  <trd lng="it">sorpresa</trd>
  <trdgrp lng="ja">
    <trd>&#x4E0D;&#x610F;&#25171;&#12385;  <pr>&#12405;&#12356;&#12358;&#12385;</pr></trd>,
    <trd>&#22855;&#35186;  <pr>&#x304D;&#12375;&#12421;&#12358;</pr></trd>,
    <trd>&#x4E0D;&#x610F;&#x306E;&#x51FA;&#26469;&#x4E8B;  <pr>&#12405;&#12356;&#x306E;&#12391;&#x304D;&#12372;&#12392;</pr></trd>,
    <trd>&#x9A5A;&#x304D;  <pr>&#x304A;&#12393;&#x308D;&#x304D;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
        <trd>verrassing</trd>,
        <trd>surprise</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>niespodzianka</trd>,
    <trd>zaskoczenie</trd>
  </trdgrp>
  <trd lng="pt">surpresa<klr tip="ind"> (a&ccedil;&atilde;o de
          surpreender)</klr></trd>
  <trdgrp lng="ru">
        <trd>&c_n;&c_je;&c_o;&c_zh;&c_i;&c_d;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
        <trd>&c_s;&c_ju;&c_r;&c_p;&c_r;&c_i;&c_z;</trd>
      </trdgrp>
  <trd lng="sk">prekvapenie</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&#x44E;&c_r;&#x43F;&c_r;&c_i;&c_z;</trd>,
    <trd>&#x43D;&c_je;&c_s;&#x43F;&#x43E;&c_d;&c_ib;&c_v;&c_a;&#x43D;&#x43A;&c_a;</trd>
  </trdgrp>
</drv>
