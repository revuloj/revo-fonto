<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>surpriz</rad>/i
</kap>
<drv mrk="surpri.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Subite kaj neatendite alveni:
      <ekz>
        la ta&ccirc;mento <tld/>is (<tld/>e atakis) la malamikon apud la
        arbaro<fnt>B</fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_z;&c_a;&c_s;&c_t;&c_a;&c_t;&c_mol; &c_v;&c_r;&c_a;&c_s;&c_p;&c_l;&c_o;&c_h;</trd>
  </snc>
  <snc>
    <uzo tip="klr">(io)</uzo>
    <dif>
      Mirigi per sia neatenditeco:
      <ekz>
        la nova&jcirc;o <tld/>is &ccirc;iujn<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_u;&c_d;&c_i;&c_v;&c_i;&c_t;&c_mol; &c_n;&c_je;&c_o;&c_zh;&c_i;&c_d;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;&c_ju;</trd>,
      <trd>&c_p;&c_o;&c_r;&c_a;&c_z;&c_i;&c_t;&c_mol; &c_n;&c_je;&c_o;&c_zh;&c_i;&c_d;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;&c_ju;</trd>
    </trdgrp>
  </snc>
  <trd lng="de">&uuml;berraschen</trd>
  <trd lng="en">surprise</trd>
  <trd lng="nl">verrassen</trd>
</drv>
</art>
