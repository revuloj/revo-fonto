<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>barbarism</rad>/o <fnt><bib>PV</bib></fnt>
  <fnt>Z</fnt>
</kap>

<drv mrk="barbar1.0o">
  <kap><tld/>o</kap>
  <snc mrk="barbar1.0o.GRA_fremdismo">
    <uzo tip="fak">GRA</uzo>
    <dif>
      Uzo de vorto ne ekzistanta en la lingvo.
    </dif>
    <trd lng="be">&c_b;&c_a;&c_r;&c_b;&c_a;&c_r;&c_y;&c_z;&c_m;</trd>
    <trd lng="ca">barbarisme</trd>
    <trd lng="de">Barbarismus</trd>
    <trd lng="es">barbarismo</trd>
    <trd lng="fr">barbarisme</trd>
    <trdgrp lng="hu">
      <trd>idegenszer&udblac;s&eacute;g<klr tip="ind">
      (nyelvi)</klr></trd>,
      <trd>barbarizmus</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>makaronizm</trd>
      <trd>nalecia&lstroke;o&sacute;ci j&eogonek;zykowe</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_a;&c_r;&c_v;&c_a;&c_r;&c_i;&c_z;&c_m;</trd>
  </snc> 
  <snc mrk="barbar1.0o.GRA_eraro">
    <uzo tip="fak">GRA</uzo>
    <dif>
      Kruda lingva malkorekta&jcirc;o.
    </dif>
    <refgrp tip="vid">
    <ref cel="soleci.0o">solecismo</ref>.
    </refgrp>  
    <trd lng="be">&c_b;&c_a;&c_r;&c_b;&c_a;&c_r;&c_y;&c_z;&c_m;</trd>
    <trdgrp lng="ca">
      <trd>solecisme</trd>,
      <trd>incorrecci&oacute;</trd>
    </trdgrp>
    <trdgrp lng="de">
       <trd>Sprachwidrigkeit</trd>,
       <trd>grober sprachlicher Fehler</trd></trdgrp>
    <trd lng="es">barbarismo</trd>
    <trd lng="fr">barbarisme</trd>
    <trdgrp lng="hu">
       <trd>nyelvront&aacute;s</trd>,
       <trd>barbarizmus<klr tip="ind"> (nyelvi)</klr></trd>
    </trdgrp>
    <trd lng="pl">barbaryzm</trd>
    <trd lng="ru">&c_v;&c_a;&c_r;&c_v;&c_a;&c_r;&c_i;&c_z;&c_m;</trd>
  </snc> 
  <snc mrk="barbar1.0o.barbareco">
    <uzo tip="stl">EVI</uzo>
    <ref tip="dif" cel="barbar.0eco">barbareco</ref>   
    <trdgrp lng="ca">
      <trd>barbaritat</trd>,
      <trd>barb&agrave;rie</trd>
    </trdgrp>
    <trd lng="de">Barbarentum</trd>
    <trd lng="es">barbaridad</trd>
    <adm>
       &Ccirc;u &ccirc;i tiu senco estas konservinda. Ekzemploj?
       <aut>MB</aut>
       Mi estas leginta la vorton iam ie kaj la etnolingvoj havas
       respondecajn tradukojn <aut>UF</aut>. 
    </adm>
  </snc> 
  <trd lng="en">barbarism</trd>
</drv>
</art>
