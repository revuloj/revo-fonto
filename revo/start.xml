<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>start</rad>/i
</kap>
<drv mrk="start.0i">
  <kap><tld/>i</kap>
  <snc mrk="start.0i.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Ekpa&scirc;i, ekkuri, ekfunkcii:
      <ekz>
        <tld/>i al kuro, al luktokonkuro.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="komenc.0i">komenci</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_r;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">starten</trd>
  <trd lng="en">start</trd>
  <trd lng="fr">d&eacute;marrer</trd>
  <trdgrp lng="hu">
    <trd>indul</trd>,
    <trd>startol</trd>
  </trdgrp>
  <trd lng="nl">starten</trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_r;&c_t;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">starta</trd>
</drv>

<drv mrk="start.0anto">
  <kap><tld/>anto</kap>
  <snc mrk="start.0anto.KOMUNE">
    <dif>
      Tiu, kiu <tld/>as, kiu partoprenas en la starto.
    </dif>
  </snc>
  <trd lng="de">Starter</trd>
  <trd lng="en">starter</trd>
  <trd lng="hu">indul&oacute;<klr tip="ind">(sport)</klr></trd>  
</drv>

<drv mrk="start.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="start.0igi.KOMUNE">
    <dif>
      Signali a&ubreve; ka&ubreve;zi <tld/>on:
      <ekz>
        mi <klr>...</klr> <tld/>igis la motoron, kaj veturis malrapide
        <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;apitro 7a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lancx.0i">lan&ccirc;i</ref>,
      <ref cel="sxalt.0i">&scirc;alti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>faire <ind>d&eacute;marrer</ind></trd>,
    <trd>donner le <ind>d&eacute;part</ind></trd>,
    <trd>mettre en marche</trd>,
    <trd>mettre en route</trd>
  </trdgrp>
</drv>
</art>
