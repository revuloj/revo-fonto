<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lup.xml,v 1.47 2021/05/29 14:32:51 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lup</rad>/o
</kap>

<drv mrk="lup.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lup.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:zoologiaj_specioj">Specio</ref> 
      de <ref tip="super" cel="hund.0o">hundo</ref> 
      (<trd lng="la">Canis lupus</trd>), 
      <ref tip="lst" cel="best.rabo0o" lst="voko:rabobestoj">rabobesto</ref>
      vivanta en la norda duonsfero kaj konsiderata kiel praulo de la 
      <ref tip="vid" cel="hund.dom0o">domhundo</ref>:
      <ekz>
        <tld/>o <tld/>on ne man&gcirc;as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        oni lin konas kiel blankan <tld/>on<fnt><bib>PrV</bib></fnt>
        <klr>(li estas konata de &ccirc;iuj)</klr>;
      </ekz>
      <ekz>
        ne veku la <tld/>on;
      </ekz>
      <ekz>
        ne elvoku <tld/>on el la arbaro
        <klr>(ne ekscitu denove trankvili&gcirc;intan pasion)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        en la versio de 1697 de <nom>Charles Perrault</nom>, la fabelo
        <klr>(ru&gcirc;kufulineto)</klr>
        fini&gcirc;as per la <tld/>o man&gcirc;ante la infanon, sed en la versio de
        la Fratoj <nom>Grimm</nom> &scirc;i estas savita
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16116">Ru&gcirc;kufulineto</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e <klr>(ege)</klr> malsati.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_o;&c_w;&c_k;</trd>
  <trd lng="bg">&c_v;&c_malmol;&c_l;&c_k;</trd>
  <trd lng="br">bleiz</trd>
  <trd lng="ca">llop</trd>
  <trd lng="cs">vlk</trd>
  <trd lng="de">Wolf</trd>
  <trd lng="el">&lambda;&ypsilon_ton;&kappa;&omikron;&sigma_fina;</trd>
  <trd lng="en">wolf</trd>
  <trd lng="es">lobo</trd>
  <trd lng="fr">loup</trd>
  <trd lng="hu">farkas</trd>
  <trd lng="id">serigala</trd>
  <trd lng="io">volfo</trd>
  <trd lng="it">lupo</trd>
  <trd lng="nl">wolf</trd>
  <trd lng="pl">wilk</trd>
  <trd lng="pt">lobo</trd>
  <trd lng="ru">&c_v;&c_o;&c_l;&c_k;</trd>
  <trd lng="sk">vlk</trd>
  <trd lng="sv">varg</trd>
  <trd lng="tr">kurt</trd>
  <trd lng="vo">lup</trd>
</drv>
<drv mrk="lup.L0o">
  <kap><tld lit="L"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt></kap>
  <mlg>Lup</mlg>
  <snc mrk="lup.L0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Sud-hemisfera <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref>
       (<trd lng="la">Lupus</trd>):
      <ekz>
        NGC 5593 estas malfermita stelamaso en konstelacio <tld lit="L"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=118656">NGC
          5593</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="nl">Wolf<klr tip="ind"> (sterrenbeeld)</klr></trd>
    <trd lng="pl">Wilk</trd>
  </snc>
  <trd lng="bo">&#xF66;&#xFA4;&#xFB1;&#xF44;&#xF0B;&#xF40;&#xF72;&#xF0B;</trd>
  <trd lng="cs">vlk</trd>
  <trd lng="fr">Loup<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="sk">vlk</trd>
</drv>

<drv mrk="lup.vir0o">
  <kap>
    vir<tld/>o
  </kap>
  <snc mrk="lup.vir0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <uzo tip="stl">RAR</uzo>
    <dif>
      Virseksa <ref cel="lup.0o" tip="super"><tld/>o</ref>.
    </dif>
    <trd lng="de">R&uuml;de<klr tip="ind"> (Wolf)</klr></trd>
    <trd lng="en">he-wolf</trd>
    <trd lng="io">volfulo</trd>
    <trdgrp lng="pl">
      <trd>wilk</trd>,
      <trd>basior <klr>(samiec dominuj&aogonek;cy w stadzie)</klr></trd>
    </trdgrp>
    <trd lng="vo">hilup</trd>
  </snc>
  <snc mrk="lup.vir0o.fantomo">
    <dif>
      <ref tip="dif" cel="lup.0fantomo"><tld lit="L"/>fantomo</ref>:
      <ekz>
        <nom>Christian Morgenstern</nom>: La Vir<tld/>o
        <fnt>
          <aut>Christian Morgenstern, trad. Bertilo Wennergren</aut>
        </fnt>.
      </ekz>
    </dif>
  </snc>
</drv>

<drv mrk="lup.0ino">
  <kap>
    <tld/>ino
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="lup.0ino.ZOO">
    <dif>
      Inseksa <ref cel="lup.0o" tip="super"><tld/>o</ref>:
      <ekz>patrinoj, similaj al <tld/>inoj, portis en la brakoj siajn
      infanojn
        <fnt>
          <aut>Henryk Sienkiewicz, trad. Lydia Zamenhof</aut>
          <vrk>Quo vadis?</vrk>
          <lok>&ccirc;apitro 56a</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i aliris al la mortinto kiel malsata <tld/>ino
        <fnt>
          <aut>Vaclav Siero&scirc;evski, trad. Kabe</aut>
          <vrk>La fundo de l'mizero</vrk>
          <lok>&ccirc;apitro 4a</lok>
        </fnt>;
      </ekz>
      <ekz>
        Romulo<klr>[n]</klr> kaj Remo<klr>[n]</klr> <klr>[&#x2026;]</klr> la
        servistoj metis en korbon, kiun 
        flosigis sur rivernivelon de inundita Tibero <klr>[&#x2026;]</klr> sed
        la akvo 
        en la rivero balda&ubreve; malalti&gcirc;is kaj la korbo kun infanoj trovi&gcirc;is
        sur tero, 
        ilia plorado allogis <tld/>inon, kiu ilin poste forportis en
        sian ku&scirc;ejon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3691">Romulo
          kaj Remo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">lloba</trd>
    <trd lng="de">W&ouml;lfin</trd>
    <trd lng="en">she-wolf</trd>
    <trd lng="es">loba</trd>
    <trd lng="fr">louve</trd>
    <trd lng="io">volfino</trd>
    <trd lng="it">lupa</trd>
    <trdgrp lng="pl">
      <trd>wilczyca</trd>,
      <trd>wadera <klr>(samica dominuj&aogonek;ca w stadzie)</klr></trd>
    </trdgrp>
    <trd lng="vo">jilup</trd>
  </snc>
  <rim>
    Ne konfuzu kun la vegeta&jcirc;o <ref tip="hom" cel="lupin.0o">lupino</ref>.
  </rim>
</drv>

<drv mrk="lup.0fantomo">
  <kap>
    <tld/>fantomo
  </kap>
  <snc>
    <dif>
      Fabela esta&jcirc;o, kiu dumtage vivas kiel homo kaj dumnokte, a&ubreve; 
      nur dum plenlunaj noktoj, kiel sangavida <tld/>o:
      <ekz>
        &ccirc;iujn maljunajn koboldojn de la unua vostoporta klaso, la
        akvofeojn 
        kaj terkoboldojn, ni devas havi, anka&ubreve; la <tld/>fantomon, la
        infer&ccirc;evalon
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la verkisto neniam vizitis Transilvanion, sed li iom konis
        legendojn kaj tradiciojn pri 
        vampiroj kaj <tld/>fantomoj, asociitaj kun tiu regiono
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2005/008717.php">Bram Stoker, kreinto
          de Dracula</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>fantomo iomete levis la lipojn kaj vidigis sian teruran
        dentaron
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Fantomurbo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
     <tld lit="L"/>fantomo malofte estas nomata anka&ubreve; &leftquot;hom<tld/>o&rightquot;,
     &leftquot;<tld/>homo&rightquot; a&ubreve; &leftquot;vir<tld/>o&rightquot;.
  </rim>
  <trd lng="de">Werwolf</trd>
  <trd lng="fr">loup-garou</trd>
</drv>


<drv mrk="lup.tasmania0o">
  <kap>tasmania <tld/>o</kap>
  <snc mrk="lup.tasmania0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Hundosimila <ref cel="best.0o" tip="super">besto</ref> el
      Tasmanio, malaperinta en la 20a jarcento
      (<trd lng="la">Thylacinus cynocephalus</trd>),
      sola <ref tip="lst" cel="speci.0o.BIO" lst="voko:zoologiaj_specioj">specio</ref> 
      de la familio tilacinedoj:
      <ekz>
        la tilacino <klr>[&#x2026;]</klr> a&ubreve; tasmania <tld/>o
        simil<klr>[e ]</klr>aspektas al
        vera lupo, sed ili du estas tiel malproksimaj
        <klr>...</klr> kiel du mamuloj  povas esti<fnt>
        <aut>S. Sarlette</aut>
        <vrk>Re: Pingvenoj kaj a&ubreve;koj</vrk>
        <lok>soc.culture.esperanto, 1998-09-28</lok>
        </fnt>;
      </ekz>
      <ekz>
        tilacino anka&ubreve; konata kiel tasmania <tld/>o a&ubreve; tasmania 
        tigro pro sia stria dorso estis la plej granda
        preda<klr>[t]</klr>a 
        marsupiulo de la nuna epoko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=299787">Tilacino</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_t;&c_a;&c_s;&c_m;&c_a;&c_n;&c_i;&c_j;&c_s;&c_k;&c_i;&c_v;&c_malmol;&c_l;&c_k;</trd>
  <trd lng="ca">llop marsupial</trd>
  <trdgrp lng="de">
    <trd>Beutelwolf</trd>,
    <trd>Tasmanischer Wolf</trd>,
    <trd>Beuteltiger</trd>,
    <trd>Tasmanischer Tiger</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>loup marsupial</trd>,
    <trd>thylacine</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>buidelwolf</trd>,
    <trd>Tasmaanse tijger</trd>
  </trdgrp>
  <trd lng="pl">diabe&lstroke; tasma&nacute;ski</trd>
  <trdgrp lng="pt">
   <trd>lobo-da-tasm&acirc;nia</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_m;&c_ch;&c_a;&c_t;&c_y;&c_j;
    <ind>&c_v;&c_o;&c_l;&c_k;</ind></trd>,
    <trd>&c_t;&c_a;&c_s;&c_m;&c_a;&c_n;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j;
    <ind>&c_v;&c_o;&c_l;&c_k;</ind></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>tigre della Tasmania</trd>, 
    <trd>tilacino</trd>
  </trdgrp>
</drv>

<drv mrk="lup.0ido">
  <kap><tld/>ido</kap>
  <snc mrk="lup.0ido.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Ido de <ref cel="lup.0o" tip="super"><tld/>o</ref>:
      <ekz>
        anka&ubreve; la du <tld/>idojn la &ccirc;asistoj aligis
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi mortigis la <tld/>idon, &ccirc;ar mi ne povis trovi &gcirc;ian patron, la
        <tld/>on
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Welpe<klr tip="ind"> (Wolf)</klr></trd>
    <trdgrp lng="fr">
      <trd>louveteau</trd>
      <trd>louvette<klr> [f.]</klr></trd> <!-- ina -->
    </trdgrp>
    <trd lng="nl">welp<klr tip="ind"> (v.wolf)</klr></trd>
    <trdgrp lng="pl">
      <trd>wilczek</trd>,
      <trd>wilcz&eogonek;</trd>,
      <trd>szczeni&eogonek; <ind>wilka</ind></trd>
    </trdgrp>
    <trd lng="ca">llobat&oacute;</trd>
    <trd lng="cs">vl&ccaron;e</trd>
    <trd lng="de">Wolfsjunges</trd>
    <trd lng="it">lupacchiotto</trd>
    <trd lng="sk">v&lacute;&ccaron;a</trd>
  </snc>
  <snc mrk="lup.0ido.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Juna (plej ofte malpli ol 11-jara) 
      <ref tip="super" cel="skolt.0o.SPO">skolto</ref>, 
      <ref tip="dif" cel="skolt.0o.SPO">skolteto</ref>:
      <ekz>
        la fondinto <klr>[lordo <nom>Baden-Powell</nom>]</klr> &ccirc;erpis la
        ideojn pri la 
        skolteta ludo el la &leftquot;Arbarega Libro&rightquot; de la brita verkisto
        <nom>Rudyard Kipling</nom>, tial skoltetoj ofte nomi&gcirc;as
        &leftquot;<tld/>idoj&rightquot; kaj la
        svarmestroj havas nomojn de bestoj
        <fnt>
          <aut>J. K. Hammer</aut>
          <vrk>Skolta kaj tenduma terminaro</vrk>
          <lok>&ccirc;apitro 6. La Svarmo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Retsesioj celantaj estrojn de Lupidoj, Skoltoj
        a&ubreve; Roveroj implikitaj en evoluo/renovigado de la Junulara
        Programo 
        <fnt>
          <aut>trad. G. Ricci</aut>
          <vrk>EURO.SCOUT.INFO 28 - Junio 2005</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>W&ouml;lfling</trd>,
      <trd>W&ouml;lflingsm&auml;dchen</trd> 
    </trdgrp>
    <trdgrp lng="en">
      <trd>cub</trd>,
      <trd><ind>cub</ind> scout</trd>,
      <trd>brownie<klr> [f.]</klr></trd> <!-- ina -->
    </trdgrp>
    <trdgrp lng="es">
      <trd>lobatos</trd>
      <trd>alita<klr> [f.]</klr></trd>, <!-- ina -->
      <trd>lobata<klr> [f.]</klr></trd> <!-- ina -->
    </trdgrp>
    <trdgrp lng="it">
      <trd>lupetto</trd>,
      <trd>coccinelle<klr> [f.]</klr></trd> <!-- ina -->
    </trdgrp>
    <trdgrp lng="nl">
      <trd>welp<klr tip="ind"> (bij scouts)</klr></trd>,
      <trd>wolfje<klr tip="ind"> (bij scouts)</klr></trd>,
      <trd>kabouter<klr tip="ind"> (bij scouts)</klr></trd>
    </trdgrp>
    <trd lng="pl">zuch</trd>
    <trd lng="pt">lobinho</trd>
  </snc>
</drv>
</art>

