<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kolubr1.xml,v 1.17 2023/12/03 09:58:53 revo Exp $">
<kap>
  <rad>kolubred</rad>/oj
</kap>
<drv mrk="kolubr1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="skvam.0uloj">skvamuloj</ref>
      (<trd lng="la">Colubridae</trd>), al kiu apartenas la plej
      multaj serpentoj, normale nedan&gcirc;eraj al la homo:
      <ekz>
        la krisopeleo a&ubreve; glis-kolubro estas genro de la familio de <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=52401">Krisopeleo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_zh;&c_o;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">col&uacute;brids </trd>
  <trd lng="de">Nattern</trd>
  <trd lng="fr">colubrid&eacute;s</trd>
  <trd lng="ja">&#x30CA;&#x30DF;&#x30D8;&#x30D3;&#x79D1;  <pr>&#x30CA;&#x30DF;&#x30D8;&#x30D3;&#x304B;</pr></trd>
  <trdgrp lng="nl">
    <trd>colubrides</trd>,
    <trd>gladde <ind>slangen</ind></trd>,
    <trd>ringslangachtigen</trd>
  </trdgrp>
  <trd lng="pl">po&lstroke;ozowate</trd>
  <trd lng="pt">colubr&iacute;deos</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_zh;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_u;&c_zh;&c_je;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;</trd>
  </trdgrp>
  <trd lng="uk">&#x432;&#x443;&#x436;&#x435;&#x432;&#x456;</trd>
  <trd lng="zh">&#x65B0;&#x86C7;&#x4E0B;&#x76EE; <pr>x&#x12B;nsh&#xE9;xi&#xE0;m&#xF9;</pr></trd>
</drv>
</art>
