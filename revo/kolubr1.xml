<vortaro>
<art mrk="$Id$">
<kap><rad>kolubred</rad>/oj</kap>
<drv mrk="kolubr1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="skvam.0uloj">skvamuloj</ref>
      (<trd lng="la">Colubridae</trd>), al kiu apartenas la plej
      multaj serpentoj, normale nedan&gcirc;eraj al la homo.
    </dif>
  </snc>
  <trd lng="de">Nattern</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_zh;&c_i; <klr>(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_u;&c_zh;&c_je;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;</trd>
  </trdgrp>
</drv>
</art>

