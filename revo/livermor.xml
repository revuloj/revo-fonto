<art mrk="$Id: livermor.xml,v 1.5 2020/03/08 08:52:06 revo Exp $">
<kap>
  <rad>livermori</rad>/o
</kap>
<drv mrk="livermor.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">KEM</uzo>
  <mlg kod="IUPAC">Lv</mlg>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
      lst="voko:&hcirc;emiaj_elementoj" val="116">elemento</ref> kun
      atomnumero 116, atommaso 293&#8239;u; radioaktiva, rapide disfalanta,
      pezega, sinteza elemento:
      <ekz>
        <tld/>o <klr>[&#8230;]</klr> ricevis sian nunan nomon la 1-an de
        decembro 2011, kiam la Internacia Unio pri Pura kaj Aplika &Hcirc;emio
        (IUPA&Hcirc;) nomumis &gcirc;in oma&gcirc;e al <nom>Livermore</nom>, la kalifornia
        urbo kie situas la fizikesplorejo
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref="https://eo.wikipedia.org/?curid=41758">Livermorio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>on eblas sintezi nur en laboratorio, kaj &gcirc;i ne estas
        trovita nature
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=29406">Elemento
          de grupo 16</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_l;&c_i;&c_v;&c_je;&c_r;&c_m;&c_o;&c_r;&c_i;&c_j;</trd>
  <trd lng="br">livermoriom</trd>
  <trd lng="ca">livermori</trd>
  <trd lng="da">livermorium</trd>
  <trd lng="de">Livermorium</trd>
  <trd lng="en">livermorium</trd>
  <trd lng="es">livermorio</trd>
  <trd lng="fr">livermorium</trd>
  <trd lng="hr">livermorij</trd>
  <trd lng="hu">livermorium</trd>
  <trd lng="id">livermorium</trd>
  <trd lng="it">livermorio</trd>
  <trd lng="la">livermorium</trd>
  <trd lng="nl">livermorium</trd>
  <trd lng="no">livermorium</trd>
  <trd lng="pl">liwermor</trd>
  <trd lng="pt">liverm&oacute;rio</trd>
  <trd lng="ru">&c_l;&c_i;&c_v;&c_je;&c_r;&c_m;&c_o;&c_r;&c_i;&c_j;</trd>
  <trd lng="sk">livermorium</trd>
  <trd lng="sr">&c_l;&c_i;&c_v;&c_je;&c_r;&c_m;&c_o;&c_r;&c_i;&#1112;&c_u;&c_m;</trd>
  <trd lng="sv">livermorium</trd>
  <trd lng="uk">&c_l;&c_ib;&c_v;&c_je;&c_r;&c_m;&c_o;&c_r;&c_ib;&c_j;</trd>
