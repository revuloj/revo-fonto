<vortaro>
<art mrk="$Id$">
<kap>
  <rad>strofoid</rad>/o
</kap>
<drv mrk="strofo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 34</lok></fnt>
  <snc>
    <dif>
      Speco de triagrada ebena <ref tip="super" cel="kurb.0o.MAT">kurbo</ref>:
      <ekz>
        la kartezia ekvacio de <tld/>o estas de la tipo
        y<sup>2</sup>(a-x)=x<sup>2</sup>(a+x).
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Strophoide</trd>
  <trd lng="en">strophoid</trd>
  <trd lng="fr">stropho&iuml;de</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_o;&c_f;&c_o;&c_i;&c_d;&c_a;</trd>
</drv>
</art>

