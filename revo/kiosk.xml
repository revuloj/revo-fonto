<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kiosk.xml,v 1.21 2022/03/21 17:51:58 revo Exp $">
<kap>
  <ofc>9</ofc><rad>kiosk</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kiosk.0o">
  <kap><tld/>o</kap>
  <snc mrk="kiosk.0o.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      &Gcirc;ardena <ref tip="vid" cel="pavilo.0o">paviloneto</ref>:
      <ekz>
        mi eliris, fumis, trinkis iun a&ccirc;a&jcirc;on en apuda trink<tld/>o,
        kaj revenis
        <fnt>
          <aut>Grigorij L. Arosev</aut>
          <vrk>Nerekonebla</vrk>
          <bib>Monato</bib>
          <lok>2001:5, p. 26a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bud.0o">budo</ref>,
      <ref cel="lauxb.0o">la&ubreve;bo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_jo;&c_s;&c_k;</trd>,
    <trd>&c_sh;&c_a;&c_p;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>alt&aacute;nek</trd>,
    <trd>kiosek</trd>,
    <trd>st&aacute;nek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gartenh&auml;uschen</trd>,
    <trd>Pavillon</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>kiosk</trd>,
    <trd>gazebo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gloriette</trd>,
    <trd>kiosque</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kioszk</trd>,
    <trd>pavilon</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>paviljoen</trd>,
    <trd>kiosk</trd>
  </trdgrp>
  <trd lng="pl">kiosk</trd>     
  <trd lng="ro">chio&scedil;c</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_o;&c_s;&c_k;</trd>,
    <trd>&c_l;&c_a;&c_r;&c_jo;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>besiedka</trd>,
    <trd>kiosk</trd>,
    <trd>st&aacute;nok</trd>
  </trdgrp>
  <trd lng="tr">k&ouml;&scedil;k</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x456;&#x43E;&#x441;&#x43A;</trd>,
    <trd>&#x43B;&#x430;&#x440;&#x44C;&#x43E;&#x43A;</trd>,
    <trd>&#x43F;&#x430;&#x432;&#x456;&#x43B;&#x44C;&#x439;&#x43E;&#x43D;</trd>
  </trdgrp>
</drv>

<drv mrk="kiosk.muzik0o">
  <kap>
    muzik<tld/>o
  </kap>
  <snc mrk="kiosk.muzik0o.ARKI">
    <dif>
      <ref tip="dif" cel="bud.muzik0o.ARKI">Muzikbudo</ref>:
      <ekz>
        la interno de la parko havas <klr>[&#x2026;]</klr> muzik<tld/>on uzatan por iuj el la
        multaj eventoj, kiuj okazas en &ccirc;i tiu parko
        <fnt>
          <aut>Matthew C.</aut>
          <vrk><url ref="https://www.meetup.com/fr-FR/Esperanto-Toronto/events/223400313/">Somera Pikniko</url></vrk>
          <lok>Grupo Esperanto-Toronto, 2015-07-18</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">kiosque &agrave; musique</trd>
</drv>
</art>
