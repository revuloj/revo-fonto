<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kiosk</rad>/o
</kap>
<drv mrk="kiosk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HOR</uzo>
  <snc>
    <dif>
      &Gcirc;ardena paviloneto:
      <ekz>
        mi eliris, fumis, trinkis iun a&ccirc;a&jcirc;on en apuda trink<tld/>o, kaj
        revenis<fnt><aut>Grigorij L. Arosev</aut>: <vrk>Nerekonebla</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 5a, p. 26a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bud.0o">budo</ref>,
      <ref cel="lauxb.0o">la&ubreve;bo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Gartenh&auml;uschen</trd>,
    <trd>Pavillon</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>kiosk</trd>,
    <trd>gazebo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gloriette</trd>,
    <trd>kiosque</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>paviljoen</trd>,
    <trd>kiosk</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_o;&c_s;&c_k;</trd>,
    <trd>&c_l;&c_a;&c_r;&c_jo;&c_k;</trd>
  </trdgrp>
</drv>
</art>
