<vortaro>
  <art mrk="$Id: salcburg.xml,v 1.9 2024/03/24 19:59:59 afido Exp $">
    <kap>
      <rad>Salcburg</rad>/o
    </kap>

<drv mrk="salcburg.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">Urbo</ref> en 
      <ref tip="super" cel="auxstr.A0ujo">A&ubreve;strujo</ref> fondita en 696 kiel 
      <ref tip="super" cel="episko.0ujo">episkopujo</ref>, situanta 
      &ccirc;e la germana limo, &ccirc;. 115&#x202F;km orient-sudoriente de <ref cel="munken.0o">Munkeno</ref>:
      <ekz>
        li ali&gcirc;is universitatajn kursojn de kristalografio en Florenco, filozofio kaj
        teologio en Romo, mezepoka historio en <tld/>o
        <fnt>
          <vrk><url ref="&Tekstaro;?nomo=mondediplo-2002">Pensi la Liberi&gcirc;on de &ccirc;iuj
          Individuoj</url></vrk>
          <lok>Le Monde diplomatique en Esperanto 2002-2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        plej grandan parton de eksterlandanoj havas Vieno (13,3&#x202F;%), <nom>Vorarlberg</nom>
        (13,3&#x202F;%)
        kaj <tld/>o (13,1&#x202F;%)
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2012/010613.php">El urboj kamparen</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_Z;&c_a;&c_l;&c_mol;&c_c;&c_b;&c_u;&c_r;&c_g;</trd>
    <trd lng="de">Salzburg</trd>
    <trd lng="en">Salzburg</trd>
    <trd lng="fr">Salzbourg</trd>
    <trd lng="nl">Salzburg</trd>
    <trd lng="pl">Salzburg</trd>
  </snc>
  <trd lng="ja">&#x30B6;&#x30EB;&#x30C4;&#x30D6;&#x30EB;&#x30B0;</trd>
  <trd lng="uk">&c_Z;&c_a;&#x43B;&#x44C;&c_c;&c_b;&c_u;&c_r;&c_gu;</trd>
  <trdgrp lng="zh">
    <trd>&#x8428;&#x5C14;&#x8328;&#x5821; <pr>S&#xE0;&#x11B;rc&#xED;b&#x1CE;o</pr></trd>,
    <trd>&#x85A9;&#x723E;&#x8328;&#x5821; <pr>S&#xE0;&#x11B;rc&#xED;b&#x1CE;o</pr></trd>,
    <trd>&#x8428;&#x5C14;&#x65AF;&#x5821; <pr>s&#xE0;&#x11B;rs&#x12B;b&#x1CE;o</pr></trd>,
    <trd>&#x85A9;&#x723E;&#x65AF;&#x5821; <pr>s&#xE0;&#x11B;rs&#x12B;b&#x1CE;o</pr></trd>
  </trdgrp>
</drv>

