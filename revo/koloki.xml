<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>kolokint</rad>/o
</kap>
<drv mrk="koloki.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Herbo el kukurbacoj (<trd lng="la">Citrullus colocynthis</trd>),
      kun globformaj, oran&gcirc;ograndaj fruktoj, kies blanka karno estas
      tre amara kaj laksiga.
    </dif>
  </snc>
  <trd lng="de">Koloquinte</trd>
  <trd lng="nl">kolokwint</trd>
</drv>
</art>

