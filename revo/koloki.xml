<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kolokint</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="koloki.0o">
  <kap><tld/>o</kap>
  <snc mrk="koloki.0o.BOT">
  <uzo tip="fak">BOT</uzo>
    <dif>
      Liano el familio <ref tip="super" cel="kukurb1.0oj">kukurbacoj</ref>
      (<trd lng="la">Citrullus colocynthis</trd>), kun globformaj,
      oran&gcirc;ograndaj fruktoj, kies blanka karno estas tre amara kaj
      laksiga.
    </dif>
  </snc>
  <trd lng="de">Koloquinte</trd>
  <trd lng="fr">coloquinte</trd>
  <trdgrp lng="hu">
    <trd>takarm&aacute;nydinnye</trd>,
    <trd>s&aacute;rt&ouml;k</trd>,
    <trd>keser&udblac; s&aacute;rt&ouml;k</trd>,
    <trd>takarm&aacute;ny-g&ouml;r&ouml;gdinnye</trd>
  </trdgrp>
  <trd lng="nl">kolokwint</trd>
  <trd lng="pt">coloqu&iacute;ntida</trd>
</drv>
</art>
