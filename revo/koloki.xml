<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kolokint</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="koloki.0o">
  <kap><tld/>o</kap>
  <snc mrk="koloki.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Liano el familio <ref tip="malprt"
      cel="kukurb1.0oj">kukurbacoj</ref>, 
      <ref tip="lst" lst="voko:botanikaj_specioj" cel="speci.0o">specio</ref>
      kun globformaj, oran&gcirc;ograndaj
      fruktoj, kies blanka karno estas tre amara kaj laksiga
      (<trd lng="la">Citrullus colocynthis</trd>):
      <ekz>
        oni uzis la <tld/>on en la antikva Romo kontra&ubreve; ron&gcirc;uloj.
      </ekz>
    </dif>
    <ref tip="vid" cel="melon.akvo0o">akvomelono</ref>
    <bld
      lok="&WCU;/thumb/3/3b/Citrullus_colocynthis_004.JPG/640px-Citrullus_colocynthis_004.JPG"
      prm="CC BY-SA 3.0"
      >folioj kaj fruktoj de <tld/>o
        <fnt>
          <aut>H. Zell, propra verko</aut>
          <lok><url ref="&WCW;/File:Citrullus_colocynthis_004.JPG"
              >https://commons.wikimedia.org/wiki/File:Citrullus_colocynthis_004.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="ca">coloquinta </trd>
  <trd lng="de">Koloquinte</trd>
  <trdgrp lng="en">
    <trd>colocynth</trd>,
    <trd>bitter apple</trd>,
    <trd>bitter cucumber</trd>,
  </trdgrp>   
  <trd lng="fr">coloquinte</trd>
  <trdgrp lng="hu">
    <trd>takarm&aacute;nydinnye</trd>,
    <trd>s&aacute;rt&ouml;k</trd>,
    <trd>keser&udblac; s&aacute;rt&ouml;k</trd>,
    <trd>takarm&aacute;ny-g&ouml;r&ouml;gdinnye</trd>
  </trdgrp>
  <trd lng="nl">kolokwint</trd>
  <trd lng="pl">arbuz kolokwinta</trd>
  <trd lng="pt">coloqu&iacute;ntida</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_l;&c_o;&c_k;&c_v;&c_i;&c_n;&c_t;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_o;&c_c;&c_i;&c_n;&c_t;</trd>
  </trdgrp>
  <trd lng="sv">kolokvint</trd>
</drv>
</art>
