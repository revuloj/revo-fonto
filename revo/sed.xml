  <trd lng="bn">&#x995;&#x9BF;&#x9A8;&#x9CD;&#x9A4;&#x9C1;</trd>
  <trd lng="bo">&#xF61;&#xF72;&#xF53;&#xF0B;&#xF53;&#xF0B;&#xF61;&#xF44;&#xF0B;</trd>
  <trdgrp lng="cs">
