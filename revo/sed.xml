  <trdgrp lng="ru">
     <trd>&c_n;&c_o;</trd>,
     <trd>&c_a;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>ale</trd>,
    <trd>av&#x161;ak</trd>,
    <trd>ke&#x10F;</trd>,
    <trd>no</trd>
  </trdgrp>
  <trd lng="sq">por</trd>
  <trd lng="sv">men</trd>
