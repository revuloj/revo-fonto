<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tili</rad>/o
</kap>
<drv mrk="tili.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super"
      cel="tiliac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Tilia</trd>),
      arbo, kies florojn oni uzas por fari kvietigajn infuzojn.
    </dif>
  </snc>
  <bld lok="../bld/tili.jpg">
    folioj de <ind>somera <tld/>o</ind>
  <trdgrp lng="hu">
    <trd>sz&eacute;leslevel&udblac; <ind>h&aacute;rs</ind></trd>,
    <trd>nagylevel&udblac; <ind>h&aacute;rs</ind></trd>
  </trdgrp>
    <trd lng="la">Tilia platyphyllos</trd>
  </bld>
  <trd lng="be">&c_l;&c_ib;&c_p;&c_a; <klr>(&c_d;&c_r;&c_e;&c_v;&c_a;)</klr></trd>
  <trd lng="bg">&c_l;&c_i;&c_p;&c_a;</trd>
  <trd lng="de">Linde</trd>
  <trd lng="en">linden</trd>
  <trd lng="fr">tilleul</trd>
  <trd lng="hu">h&aacute;rs(fa)</trd>
  <trd lng="nl">linde</trd>
  <trd lng="pl">lipa</trd>
  <trdgrp lng="pt">
    <trd>t&iacute;lia</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_i;&c_p;&c_a;</trd>
  <trd lng="tr">&inodot;hlamur</trd>
</drv>
</art>
