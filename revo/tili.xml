<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tili.xml,v 1.29 2023/09/04 19:01:52 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tili</rad>/o
</kap>
<drv mrk="tili.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio 
      <ref tip="malprt" cel="malvac.0oj">malvacoj</ref>
      (<trd lng="la">Tilia</trd>),
      arbo, kies florojn oni uzas por fari kvietigajn infuzojn:
      <ekz>
        ili ekploris, kiam ili vidis, ke la birdo mortis, 
        <klr>[&#x2026;]</klr> fosis por la birdo beletan tombon, kiun ili ornamis per folietoj de <tld/>oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Lekanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        seki&gcirc;inta, ruli&gcirc;inta folio de <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Elfo de la rozo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>oj estis kovritaj de flora&jcirc;o
        <fnt>
          <bib>Fab2</bib>
          <lok>Abio</lok>
        </fnt>;
      </ekz>
      <ekz>
        altaj <tld/>oj, kiuj gracie kaj bele staris alte &gcirc;is la
        supro de la tegmento kaj plenigis la aeron per plej dol&ccirc;a bonodoro
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/8/82/Tilia_platyphyllos_%287282550976%29.jpg/640px-Tilia_platyphyllos_%287282550976%29.jpg" prm="CC BY 2.0">floroj kaj folioj de <ind>somera <tld/>o</ind> (Tilia platyphyllos)
         <fnt>
           <aut>Radio Tonreg de Vieno, A&ubreve;strujo, foto provizita de Jacopo Werther</aut>
           <lok><url ref="&WCW;/File:Tilia_platyphyllos_(7282550976).jpg">&WCW;/File:Tilia_platyphyllos_(7282550976).jpg</url></lok>
         </fnt>
      <trdgrp lng="hu">
        <trd>sz&eacute;leslevel&udblac; <ind>h&aacute;rs</ind></trd>,
        <trd>nagylevel&udblac; <ind>h&aacute;rs</ind></trd>
      </trdgrp>
      <trd lng="la">Tilia platyphyllos</trd>
    </bld>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_p;&c_a; <klr>(&c_d;&c_r;&c_e;&c_v;&c_a;)</klr></trd>
  <trd lng="bg">&c_l;&c_i;&c_p;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>til&middot;ler</trd>,
    <trd>tell</trd>
  </trdgrp>
  <trd lng="de">Linde</trd>
  <trd lng="en">linden</trd>
  <trd lng="fr">tilleul</trd>
  <trd lng="hu">h&aacute;rs(fa)</trd>
  <trdgrp lng="ja">
    <trd>&#x30DC;&#x30C0;&#x30A4;&#x30B8;&#x30E5;</trd>,
    <trd>&#x30B7;&#x30CA;&#x30CE;&#x30AD;</trd>
  </trdgrp>
  <trd lng="nl">linde</trd>
  <trd lng="pl">lipa</trd>
  <trdgrp lng="pt">
    <trd>t&iacute;lia</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_i;&c_p;&c_a;</trd>
  <trd lng="tr">&inodot;hlamur</trd>
  <trd lng="sk">lipa</trd>
  <trd lng="cs">l&iacute;pa</trd>
  <trd lng="uk">&#x43B;&#x438;&#x43F;&#x430;</trd>
</drv>
</art>
