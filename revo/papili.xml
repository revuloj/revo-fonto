<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc><rad>papili</rad>/o</kap>

<drv mrk="papili.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Iu el la ordo <ref tip="malprt" cel="lepido1.0oj">lepidopteroj</ref>,
      insekto kun kvar flugiloj kovritaj per diverskoloraj skvametoj tiel
      subtilaj kiel polvo:
      <ekz>
        senpripensa kiel <tld/>o;
      </ekz>
      <ekz>
        <tld/>a <klr>(neserioza kaj malkonstanta)</klr> amo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e <klr>(senatente)</klr> tralegi libron..
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ir.rond0i">rondiri</ref>.
    </refgrp>
  </snc>         
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_t;&c_y;&c_l;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_t;&c_y;&c_l;&c_jo;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_je;&c_p;&c_je;&c_r;&c_u;&c_d;&c_a;</trd>
  <trdgrp lng="br">
    <trd>balafenn</trd>,
    <trd>pobelan</trd>
  </trdgrp>                             
  <trd lng="de">Schmetterling</trd>
  <trd lng="fr">papillon</trd>
  <trdgrp lng="hu">
    <trd>lepke</trd>,
    <trd>pillang&oacute;</trd>
  </trdgrp>
  <trd lng="nl">vlinder</trd>
  <trdgrp lng="pt">
    <trd>borboleta</trd>,
    <trd>mariposa</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_b;&c_o;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_o;&c_t;&c_y;&c_l;&c_jo;&c_k;</trd>
  </trdgrp>
</drv>
</art>
