    <drv mrk="lakt.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="lakt.0o.mambesto">
        <uzo tip="fak">KUI</uzo>
        <dif>
          Blanka, dol&ccirc;a flua&jcirc;o, produktata de specialaj
          glandoj de la inaj mambestoj por la nutrado de la idoj:
          <ekz>
            kaprina <tld/>o
            <fnt><bib>MT</bib>, <lok>Sentencoj 27:27</lok></fnt>;
          </ekz>
          <ekz>
            <tld/>a dieto;
          </ekz>
          <ekz>
            havi ankora&ubreve; la <tld/>on sur la
            lipoj<fnt><bib>PrV</bib></fnt>
            <klr>(esti juna kaj sensperta)</klr>;
          </ekz>
          <ekz>
            nenia peno nek provo donus <tld/>on de
            bovo<fnt><bib>PrV</bib></fnt>.
          </ekz>
        </dif>
        <ref tip="lst" cel="trink.0ajxo" lst="voko:trinka&jcirc;oj">trinka&jcirc;o</ref>
        <refgrp tip="vid">
          <ref cel="kaze.0o">kazeo</ref>,
          <ref cel="jahurt.0o">jahurto</ref>,
          <ref cel="fromag.0o">froma&gcirc;o</ref>,
          <ref cel="buter.0o">butero</ref>,
          <ref cel="krem.0o">kremo</ref>.
        </refgrp>
        <trd lng="pl">mleko</trd>
      </snc>
      <snc mrk="lakt.0o.similanta">
        <uzo tip="fak">KUI</uzo>
        <dif>
          Nomo de kelkaj flua&jcirc;oj similantaj <tld/>on:
          <ekz>
            migdala, kokosa <tld/>o.</ekz>
        </dif>
        <trd lng="pl">mleczko</trd>
      </snc>
