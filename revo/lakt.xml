<drv mrk="lakt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="lakt.0i.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      <ref tip="super" cel="sekrec.0i.FIZL">Sekrecii</ref> <tld/>on:
      <ekz>
        por unu talento <klr>[&#8230;]</klr> oni povas havi <klr>[&#8230;]</klr> sesdek belajn
        <tld/>antajn bovinojn, a&ubreve; dek sklavojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="melk.0i">melki</ref>
  <trd lng="be">&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; &c_m;&c_a;&c_l;&c_a;&c_k;&c_o;</trd>
  <trd lng="br">rei&ntilde; <ind>laezh</ind></trd>
  <trdgrp lng="ca">
    <trd>lactar</trd>,
    <trd>alletar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dojit</trd>,
    <trd>d&aacute;vat ml&eacute;ko</trd>
  </trdgrp>
  <trd lng="de">Milch geben</trd>
  <trd lng="en">lactate</trd>
  <trd lng="fr">donner du lait</trd>
  <trd lng="hu">tejel</trd>
  <trdgrp lng="id">
    <trd><baz>susu</baz>bersusu</trd>,
    <trd><baz>susu</baz>menyusu</trd>
  </trdgrp>
  <trd lng="ja">&#x4E73;&#12434;&#x51FA;&#12377;  <pr>&#12385;&#12385;&#12434;&#12384;&#12377;</pr></trd>
  <trdgrp lng="pl">
    <trd>mleczny</trd>,
    <trd>mlekodajny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_k;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol; &c_m;&c_o;&c_l;&c_o;&c_k;&c_o;<klr> (&c_o; &c_k;&c_o;&c_r;&c_o;&c_v;&c_je; &c_i; &c_t;.&c_p;.)</klr></trd>,
    <trd>&c_d;&c_o;&c_i;&c_t;&c_mol;&c_s;&c_ja;<klr> (&c_o; &c_k;&c_o;&c_r;&c_o;&c_v;&c_je; &c_i; &c_t;.&c_p;.)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>doji&tcaron;</trd>,
    <trd>d&aacute;va&tcaron; mlieko</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_o;&c_ji;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &c_m;&c_o;&c_l;&c_o;&c_k;&c_o; (&c_p;&c_r;&c_o; &c_k;&c_o;&c_r;&c_o;&c_v;&c_u; &c_ib; &c_t;.&c_p;.)</trd>
  </trdgrp>
</drv>

<drv mrk="lakt.0eca">
  <kap><tld/>eca, <var><kap><tld/>a</kap></var></kap>
  <snc mrk="lakt.0eca.apartenanta">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        denove venos sur nian tablon <tld/>aj man&gcirc;a&jcirc;oj, butero kaj froma&gcirc;o
        <fnt>
          <bib>Fab4</bib>
          <lok>Kion la patro faras, estas &ccirc;iam &gcirc;usta</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Milch-</trd>
    <trdgrp lng="hu">
      <trd>tej-</trd>,
      <trd>tejes</trd>
    </trdgrp>
    <trd lng="fr">lactique</trd>
    <trdgrp lng="pl">
      <trd>mleczny</trd>,
      <trd>mlekowy</trd>
    </trdgrp>
  </snc>
  <snc mrk="lakt.0eca.simila">
    <dif>
      Simila al <tld/>o:
      <ekz>
        la &ccirc;ielo fari&gcirc;is cindrokolora, poste flavru&gcirc;a, fine <tld/>a
        <klr>(kp <ref cel="diafan.0a">diafana</ref>)</klr>
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz><ekz>
        la plafono estis porcelansimila surfaco kaj la sunlumo,
        filtrata tra la <tld/>aj vitromuroj fakte donis la plej perfektan lumigon
        <fnt><bib>VaK</bib></fnt>;
      </ekz><ekz>
        la <tld/>eca muko de la salamandro faris katastrofan makulon sur la bela &scirc;uo
        <fnt><bib>KKr</bib></fnt>;
      </ekz><ekz>
        oni faris la sensoifigan blankan bieron el &ccirc;irka&ubreve; 50 procentoj da nemaltigita
        tritiko, pro tio estas ties <tld/>eca, natura malklareco
       <fnt>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="&Monato;/2001/007144.php">Bierlando Belgio</url></vrk>
          <bib>Monato</bib>
          <lok>2001/01, p. 18</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="el">&gamma;&alfa;&lambda;&alfa;&kappa;&tau;&omega_ton;&delta;&eta;&sigma_fina;</trd>
    <trd lng="de">milchig</trd>
    <trd lng="en">milky</trd>
    <trd lng="fr">laiteux</trd>
    <trd lng="hu">tejszer&udblac;</trd>
    <trd lng="nl">melkachtig</trd>
    <trd lng="pl">mleczny</trd>
    <trd lng="pt">leitoso</trd>
    <trd lng="tr">s&uuml;t gibi</trd>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_o;&c_ch;&c_n;&c_y;</trd>
  <trd lng="br">laezhek</trd>
  <trdgrp lng="ca">
    <trd>llet&oacute;s</trd>,
    <trd>lacti</trd>,
    <trd>l&agrave;ctic</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>l&aacute;cteo</trd>,
    <trd>l&aacute;ctea</trd>
  </trdgrp>
  <trd lng="ja">&#x4E73;&#x72B6;&#x306E;  <pr>&#12385;&#12385;&#12376;&#12423;&#12358;&#x306E;</pr></trd>
  <trd lng="ru">&c_m;&c_o;&c_l;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_o;&c_l;&c_o;&c_ch;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_m;&c_o;&c_l;&c_o;&c_k;&c_o;&c_p;&c_o;&c_d;&c_ib;&c_b;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="zh">&#x4E73;&#x767D;&#33394; <pr>r&#x1D4;b&#xE1;is&#xE8;</pr></trd>
</drv>

<drv mrk="lakt.0umo">
  <kap><ofc>*</ofc><tld/>umo
    <fnt>
      <bib>F</bib>, <lok>UniversalaVortaro,
      <url ref="&AdE;/fundamento/universala_vortaro.html#av-5083">lakt'um'</url></lok>
    </fnt>
  </kap>
  <snc mrk="lakt.0umo.CXAS">
    <dif>
      Semo de viraj fi&scirc;oj, kiu fruktigas la ovojn:
    </dif>
    <ekz>
      mi tute konsentas kun vi, ke <tld/>o kaj <tld/>umo havas inter
      si preska&ubreve; nenion komunan
      <fnt><bib>LR</bib></fnt>.
    </ekz>
    <refgrp tip="vid">
      <ref cel="fraj.0o">frajo</ref>,
      <ref cel="kaviar.0o">kaviaro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_a;&c_k;&c_o; <klr>(&c_u; &c_r;&c_y;&c_b;)</klr></trd>
  <trd lng="br">laezhenn</trd>
  <trdgrp lng="ca">
    <trd>llet</trd>,
    <trd>l&iacute;quid seminal dels peixos</trd>
  </trdgrp>
  <trd lng="cs">ryb&iacute; ml&iacute;&ccaron;&iacute;</trd>
  <trdgrp lng="de">
    <trd>Fischmilch</trd>,
    <trd>Milch <klr>(vom Fisch)</klr></trd>
  </trdgrp>
  <trd lng="en">milt</trd>
  <trd lng="es">lecha</trd>
  <trd lng="fr">laitance</trd>
  <trd lng="hu">haltej</trd>
  <trd lng="id"><ind>sperma</ind> ikan</trd>
  <trd lng="ja">&#12375;&#12425;&#12371;</trd>
  <trd lng="nl">hom</trd>
  <trd lng="pl">mlecz <klr>(ryby)</klr></trd>
  <trd lng="ru">&c_m;&c_o;&c_l;&c_o;&c_k;&c_i;</trd>
  <trd lng="sk">mlie&ccaron;ie <klr>(rybacie sperma)</klr></trd>
  <trd lng="tr">s&uuml;t</trd>
  <trd lng="uk">&c_m;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_o; (&c_u; &c_r;&c_i;&c_b;)</trd>
</drv>

<drv mrk="lakt.0avojo">
  <kap><tld lit="L"/>a Vojo<fnt><bib>PV</bib></fnt>,
    <var><kap><tld lit="L"/>ovojo<fnt><bib>PIV2</bib></fnt></kap></var>
  </kap>
  <snc mrk="lakt.0avojo.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Nia <ref tip="super" cel="galaksi.0o.stelamaso">galaksio</ref>
      enhavanta nian <ref tip="prt" cel="sun.0sistemo">sunsistemon</ref>:
      <ekz>
        de sur Tero la <tld lit="L"/>a Vojo aspektas kiel lar&gcirc;a
        blanketa strio videbla nokte en &ccirc;ielo,
        konsistanta el malproksimegaj sennombraj steloj;
      </ekz><ekz>
        la galaksioj NGC 4302 kaj NGC 4298 <klr>[&#8230;]</klr> amba&ubreve; estas malpli grandaj ol
        la nia, la <tld lit="L"/>a Vojo
        <fnt><bib>Kon11</bib></fnt>;
      </ekz><ekz>
        ni vidis la Sudan Krucon kaj la <tld lit="L"/>an Vojon kun la du malgrandaj
