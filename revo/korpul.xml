<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>korpulent</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="korpul.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Korpampleksa, vastkorpa, dikventra:
      <ekz>
        interne sidis la <tld/>a figuro de virino de matura
        a&gcirc;o<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
     <trd>&c_m;&c_a;&c_zh;&c_n;&c_y;</trd>,
     <trd>&c_t;&c_l;&c_u;&c_s;&c_t;&c_y;</trd>,
     <trd>&c_g;&c_l;&c_a;&c_d;&c_k;&c_ib;</trd>,
     <trd>&c_p;&c_o;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>korpulent</trd>,
     <trd>beleibt</trd>,
     <trd>wohlbeleibt</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>corpulent</trd>,
    <trd>fat</trd>,
    <trd>fleshy</trd>,
    <trd>obese</trd>,
    <trd>stout</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_r;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_u;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
