<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: brancx.xml,v 1.71 2021/06/18 20:41:26 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bran&ccirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="brancx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="brancx.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="tig.0o">Tigo</ref> kreskanta
      el alia tigo a&ubreve; el trunko:
      <ekz>
        &gcirc;i <klr>...</klr> pendi&gcirc;is sur la <tld/>oj de la
        arbeto 
        <fnt><bib>F</bib>, <lok>&FE; 39</lok></fnt>;
      </ekz>
      <ekz>
        prenu al vi en la unua tago fruktojn de belaj arboj,
        <tld/>ojn de palmoj kaj <tld/>ojn de densaj arboj kaj de
        apudriveraj salikoj
        <fnt><bib>MT</bib>, <lok>&Lev; 23:40</lok></fnt>;
      </ekz>
      <ekz>
        la trunko havis tri <tld/>ojn
        <fnt><bib>MT</bib>, <lok>&Gen; 40:10</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="baston.0o">bastono</ref>,
      <ref cel="verg.0o">vergo</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_g;&c_a;&c_l;&c_ib;&c_n;&c_a;</trd>,
      <trd>&c_g;&c_a;&c_l;&c_ib;&c_n;&c_k;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Ast</trd>,
      <trd>Zweig</trd>
    </trdgrp>
    <trd lng="hu">&aacute;g</trd>
    <trd lng="pl">ga&lstroke;&aogonek;&zacute;</trd>
    <trd lng="pt">galho</trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_je;&c_t;&c_v;&c_mol;</trd>,
      <trd>&c_v;&c_je;&c_t;&c_k;&c_a;</trd>,
      <trd>&c_s;&c_u;&c_k;</trd>
    </trdgrp>
  </snc>
  <snc mrk="brancx.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Io, kio disaparti&gcirc;as el la &ccirc;efparto, kiel <tld/>o el
      trunko; divida&jcirc;o, fako:
      <ekz>
        la <tld/>oj de rivero;
      </ekz>
      <ekz>
        la instruado estu fundamenta, lar&gcirc;a, plena, <klr>...</klr>
        &gcirc;i ampleksu &ccirc;iujn <tld/>ojn de la koncernata objekto
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
      <ekz>
        la <tld/>oj de la montaro<fnt>B</fnt>;
      </ekz>
      <ekz>
        la ses <tld/>oj kiuj elstaris el la kandelabro
        <fnt><bib>MT</bib>, <lok>&Eli; 37:20</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="al1.0o">alo</ref>,
      <ref cel="brak.0o.FIG">brako <sncref/></ref>,
      <ref cel="fili.0o">filio</ref>,
      <ref cel="fork.0o">forko</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_g;&c_a;&c_l;&c_ib;&c_n;&c_a;
      <klr>(&c_n;&c_a;&c_v;&c_u;&c_k;&c_ib;, &c_p;&c_r;&c_a;&c_m;&c_y;&c_s;&c_l;&c_o;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_ib; &c_ib; &c_d;&c_a;
      &c_g;.&c_p;.)</klr></trd>,
      <trd>&c_a;&c_d;&c_g;&c_a;&c_l;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Arm</trd>,
      <trd>Zweig</trd>,
      <trd>Branche</trd>,
      <trd>Ausl&auml;ufer</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&aacute;g</trd>,
      <trd>&aacute;gazat</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>ga&lstroke;&aogonek;&zacute;</trd>,
      <trd>bran&zdot;a</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_je;&c_t;&c_v;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_v;&c_je;&c_t;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_o;&c_t;&c_r;&c_a;&c_s;&c_l;&c_mol; <klr>(&c_n;&c_a;&c_u;&c_k;&c_i;,
      &c_p;&c_r;&c_o;&c_m;&c_y;&c_sh;&c_l;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_i;)</klr></trd>,
      <trd>&c_r;&c_u;&c_k;&c_a;&c_v; <klr>(&c_r;&c_je;&c_k;&c_i;)</klr></trd>,
      <trd>&c_o;&c_t;&c_r;&c_o;&c_g; <klr>(&c_g;&c_o;&c_r;&c_n;&c_o;&c_j; &c_c;&c_je;&c_p;&c_i;)</klr></trd>,
      <trd>&c_r;&c_a;&c_z;&c_r;&c_ja;&c_d; <klr>(&c_v; &c_k;&c_l;&c_a;&c_s;&c_s;&c_i;&c_f;&c_i;&c_k;&c_a;&c_c;&c_i;&c_i;)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="af">tak</trd>
  <trd lng="am">&#x1245;&#x122D;&#x1295;&#x132B;&#x134D;</trd>
  <trd lng="ar">&#x641;&#x631;&#x639;</trd>
  <trd lng="az">filial&#x131;</trd>
  <trd lng="bg">&c_k;&c_l;&c_o;&c_n;</trd>
  <trd lng="bn">&#x9B6;&#x9BE;&#x996;&#x9BE;</trd>
  <trdgrp lng="br">
    <trd>barr</trd>,
    <trd>brank</trd>,
    <trd>skourr</trd>
  </trdgrp>
  <trd lng="bs">ogranak</trd>
  <trd lng="co">parus</trd>
  <trd lng="da">gren</trd>
  <trd lng="es">rama</trd>
  <trd lng="et">filiaali</trd>
  <trd lng="eu">adarra</trd>
  <trd lng="fr">branche</trd>
  <trd lng="fy">t&#xFB;ke</trd>
  <trd lng="gl">sector</trd>
  <trd lng="gu">&#xAB6;&#xABE;&#xA96;&#xABE;</trd>
  <trd lng="ha">reshe</trd>
  <trd lng="he">&#x5E2;&#x5E0;&#x5E3;</trd>
  <trd lng="hi">&#x936;&#x93E;&#x916;&#x93E;</trd>
  <trd lng="hr">grana</trd>
  <trd lng="hy">&#x574;&#x561;&#x57D;&#x576;&#x561;&#x573;&#x575;&#x578;&#x582;&#x572;</trd>
  <trd lng="is">&#xFA;tib&#xFA;</trd>
  <trd lng="ja">&#x30D6;&#x30E9;&#x30F3;&#x30C1;</trd>
  <trd lng="jw">cabang</trd>
  <trd lng="ka">&#x10E4;&#x10D8;&#x10DA;&#x10D8;&#x10D0;&#x10DA;&#x10D8;</trd>
  <trd lng="km">&#x179F;&#x17B6;&#x1781;&#x17B6;</trd>
  <trd lng="ko">&#xC9C0;&#xC810;</trd>
  <trd lng="lo">&#xEAA;&#xEB2;&#xE82;&#xEB2;</trd>
  <trd lng="ml">&#xD36;&#xD3E;&#xD16;</trd>
  <trd lng="mr">&#x936;&#x93E;&#x916;&#x93E;</trd>
  <trd lng="my">&#x1021;&#x1000;&#x102D;&#x102F;&#x1004;&#x103A;&#x1038;</trd>
  <trd lng="ne">&#x936;&#x93E;&#x916;&#x93E;</trd>
  <trd lng="ny">nthambi</trd>
  <trd lng="pa">&#xA38;&#xA3C;&#xA3E;&#xA16;&#xA3E;</trd>
  <trd lng="ps">&#x685;&#x627;&#x646;&#x6AB;&#x647;</trd>
  <trd lng="sd">&#x634;&#x627;&#x62E;</trd>
  <trd lng="si">&#xDC1;&#xDCF;&#xD9B;&#xDCF;</trd>
  <trd lng="sk">vetva</trd>
  <trd lng="sq">deg&#xEB;</trd>
  <trd lng="st">ea lekala</trd>
  <trd lng="ta">&#xB95;&#xBBF;&#xBB3;&#xBC8;</trd>
  <trd lng="te">&#xC36;&#xC3E;&#xC16;</trd>
  <trd lng="th">&#xE2A;&#xE32;&#xE02;&#xE32;</trd>
  <trd lng="tl">sangay</trd>
  <trd lng="ur">&#x634;&#x627;&#x62E;</trd>
  <trd lng="vi">chi nh&#xE1;nh</trd>
  <trd lng="yi">&#x5E6;&#x5D5;&#x5D5;&#x5D9;&#x5D9;&#x5B7;&#x5D2;</trd>
  <trd lng="zu">igatsha</trd>
</drv>

<drv mrk="brancx.0ajxo">
  <kap><tld/>a&jcirc;o<fnt>Z</fnt></kap>
  <snc>
    <dif>
      <tld lit="B"/>ofasko.
    </dif>
  </snc>
  <trd lng="es">ramaje</trd>
  <trd lng="fr">branchage</trd>
  <trd lng="he">&#x5E2;&#x5E0;&#x5E3;</trd>
  <trd lng="pl">chrust</trd>
</drv>

<drv mrk="brancx.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="brancx.0aro.BOT">
    <dif>
      La bran&ccirc;oj de unu a&ubreve; pluraj vegeta&jcirc;oj, precipe
      arboj:
      <ekz>
        &ccirc;i tiu arbo <klr>...</klr> lukse disvolvas sian densan
        <tld/>aron
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">corona<klr> de un &aacute;rbol</klr></trd>
  <trd lng="fr">branchage</trd>
  <trd lng="pl">korona<klr> drzewa</klr></trd>
</drv>

<drv mrk="brancx.dis0igxi">
  <kap>dis<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="brancx.dis0igxi.KOMUNE">
    <dif>
      Disdividi&gcirc;i:
      <ekz>
        sur la tombejo en Lu&ccirc;ica estis granda
        dis<tld/>i&gcirc;inta tilio
        <fnt>
          <aut>E. Kumi&ccaron;i&cacute;, trad. S. Rukelj</aut>,
          <vrk>
          <url ref="http://free-vz.t-com.hr/Stanko/Surprizitaj.pdf">
          Surprizitaj nuptofestantoj</url></vrk>,
          <lok>inko, 2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_g;&c_a;&c_l;&c_ib;&c_n;&c_o;&c_w;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="br">
    <trd>gaolia&ntilde;<klr tip="ind"> (gwez)</klr></trd>,
    <trd>gaoli&ntilde;</trd>
  </trdgrp>
  <trd lng="de">sich <ind>verzweigen</ind></trd>
  <trdgrp lng="es">
    <trd><ind>ramificar</ind>se</trd>,
    <trd><ind>subdividir</ind>se</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>ramifier</ind></trd>,
    <trd>se <ind>subdiviser</ind></trd>,
    <trd>se <ind>s&eacute;parer</ind></trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5D4;&#x5E1;&#x5EA;&#x5E2;&#x5E3;</trd>
  <trdgrp lng="hu">
    <trd>el&aacute;gazik</trd>,
    <trd>sz&eacute;t&aacute;gazik</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rozga&lstroke;&eogonek;zia&cacute;</trd>,
    <trd>odga&lstroke;&eogonek;zia&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_v;&c_je;&c_t;&c_v;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="sk">rozvetvi&tcaron; sa</trd>
</drv>
</art>
