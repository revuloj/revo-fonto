<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>bran&ccirc;</rad>/o
</kap>
<drv mrk="brancx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="brancx.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="tig.0o">Tigo</ref> kreskanta
      el alia tigo (a&ubreve; trunko):
      <ekz>
        prenu al vi en la unua tago fruktojn de belaj arboj,
        <tld/>ojn de palmoj kaj <tld/>ojn de densaj arboj kaj de
        apudriveraj salikoj
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Levidoj 23, 40</lok>)</fnt>;
      </ekz>
      <ekz>
        la trunko havis tri <tld/>ojn
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Genezo 40, 10</lok>)</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="verg.0o">vergo</ref>,
      <ref cel="baston.0o">bastono</ref>.
    </refgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_je;&c_t;&c_v;&c_mol;</trd>,
      <trd>&c_v;&c_je;&c_t;&c_k;&c_a;</trd>,
      <trd>&c_s;&c_u;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Ast</trd>,
      <trd>Zweig</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Io, kio disaparti&gcirc;as el la &ccirc;efparto, kiel <tld/>o el
      trunko; divida&jcirc;o, fako:
      <ekz>
        la diversaj <tld/>oj de
        scienco<fnt>Z</fnt>, komerco;
      </ekz>
      <ekz>
        la <tld/>oj de rivero;
      </ekz>
      <ekz>
        la <tld/>oj de la montaro<fnt>B</fnt>;
      </ekz>
      <ekz>
        la ses <tld/>oj kiuj elstaris el la kandelabro
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Eliro 37, 20</lok>)</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="al1.0o">alo</ref>,
      <ref cel="brak.0o">brako</ref>,
      <ref cel="fork.0o">forko</ref>,
      <ref cel="fili.0o">filio</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Arm</trd>,
      <trd>Zweig</trd>,
      <trd>Branche</trd>,
      <trd>Ausl&auml;ufer</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_je;&c_t;&c_v;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_v;&c_je;&c_t;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_o;&c_t;&c_r;&c_a;&c_s;&c_l;&c_mol;
      <klr>(&c_n;&c_a;&c_u;&c_k;&c_i;,
      &c_p;&c_r;&c_o;&c_m;&c_y;&c_sh;&c_l;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_i;)</klr></trd>,
      <trd>&c_r;&c_u;&c_k;&c_a;&c_v; <klr>(&c_r;&c_je;&c_k;&c_i;)</klr></trd>,
      <trd>&c_o;&c_t;&c_r;&c_o;&c_g; <klr>(&c_g;&c_o;&c_r;&c_n;&c_o;&c_j;
      &c_c;&c_je;&c_p;&c_i;)</klr></trd>,
      <trd>&c_r;&c_a;&c_z;&c_r;&c_ja;&c_d; <klr>(&c_v;
      &c_k;&c_l;&c_a;&c_s;&c_s;&c_i;&c_f;&c_i;&c_k;&c_a;&c_c;&c_i;&c_i;)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="es">rama</trd>
  <trd lng="fr">branche</trd>
</drv>
<drv mrk="brancx.0ajxo">
  <kap><tld/>a&jcirc;o<fnt>Z</fnt></kap>
  <snc>
    <dif>
      <tld lit="B"/>fasko.
    </dif>
  </snc>
  <trd lng="fr">branchage</trd>
</drv>
<drv mrk="brancx.dis0igxi">
  <kap>dis<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Disdividi&gcirc;i.
    </dif>
  </snc>
  <trd lng="de">sich <ind>verzweigen</ind></trd>
  <trdgrp lng="fr">
    <trd>se <ind>ramifier</ind></trd>,
    <trd>se <ind>subdiviser</ind></trd>,
    <trd>se <ind>s&eacute;parer</ind></trd></trdgrp> 
  <trd
  lng="ru">&c_r;&c_a;&c_z;&c_v;&c_je;&c_t;&c_v;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>
</art>
