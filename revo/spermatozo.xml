<art mrk="$Id: spermatozo.xml,v 1.13 2023/09/04 05:14:57 revo Exp $">
<kap>
  <rad>spermatozo</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="spermatozo.0o">
  <kap><tld/>o</kap>
  <snc mrk="spermatozo.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      Matura virseksa
      <ref tip="super" cel="cxel.0o.BIO">&ccirc;elo<sncref/></ref>:
      <ekz>
        <nom>Antoni van Leeuwenhoek</nom> <klr>[&#x2026;]</klr> estis la unua kiu observis kaj priskribis
        muskolajn fibrojn, bakteriojn, <tld/>ojn kaj sangan fluon en kapilaroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19377">Antoni van Leeuwenhoek</url></lok>
        </fnt>.
      </ekz>      
    </dif>
    <trdgrp lng="de">
      <trd>Spermium </trd>,
      <trd>Spermatozoon</trd>,
      <trd>Samenfaden</trd>,
      <trd>Samenzelle</trd>
    </trdgrp>
  </snc>
  <refgrp tip="vid">
    <ref cel="cxel.ovo0o.FIZL">ovo&ccirc;elo</ref>
  </refgrp>
  <trd lng="be">&c_s;&c_p;&c_e;&c_r;&c_m;&c_a;&c_t;&c_a;&c_z;&c_o;&c_ib;&c_d;</trd>
  <trd lng="bg">&c_s;&c_p;&c_je;&c_r;&c_m;&c_a;&c_t;&c_o;&c_z;&c_o;&c_i;&c_d;</trd>
  <trd lng="ca">espermatozou</trd>
  <trdgrp lng="en">
    <trd>spermatozoon</trd>,
    <trd>sperm</trd>
  </trdgrp>
  <trd lng="es">espermatozoide</trd>
  <trd lng="fr">spermatozo&iuml;de</trd>
  <trd lng="ja">&#x7CBE;&#x5B50;  [<ind>&#x305B;&#x3044;&#x3057;</ind>]</trd>
  <trd lng="nl">spermatozo&iuml;de</trd>
  <trd lng="ru">&c_s;&c_p;&c_je;&c_r;&c_m;&c_a;&c_t;&c_o;&c_z;&c_o;&c_i;&c_d;</trd>
  <trd lng="sk">spermia</trd>
  <trdgrp lng="cs">
    <trd>spermatozoon</trd>,
    <trd>spermie</trd>
  </trdgrp>
</drv>
</art>

