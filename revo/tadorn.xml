<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tadorn.xml,v 1.3 2022/03/28 12:03:03 revo Exp $">
<kap>
  <rad>tadorn</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tadorn.0o">
  <kap><tld/>o</kap>
  <snc mrk="tadorn.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Iu el diversaj grandaj anaso-similaj
      <ref tip="lst" cel="bird.nagxo0o" lst="voko:birdoj">na&gcirc;birdoj</ref>;
      genro el familio
      <ref tip="malprt" cel="anased.0oj"><tld/>edoj</ref>
      <ekz>
        li enakvi&gcirc;is kaj trenis la sitelon kun iom da restanta akvo, 
        kaj la sitelo preska&ubreve; frakasis tiun senhontan <tld/>on
        <fnt>
          <aut>Mo Yan, trad. Vejdo</aut>
          <vrk>Travidebla Ru&gcirc;a Rafano</vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">tadorne</trd>
    <trd lng="la">Tadorna</trd>
    <trdgrp lng="pl">
        <trd>ohar</trd>,
        <trd>kaczka norowa</trd>
    </trdgrp>
  </snc>
  
  <trd lng="uk">&#x433;&#x430;&#x43B;&#x430;&#x433;&#x430;&#x437;</trd>
</drv>
</art>

