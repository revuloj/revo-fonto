<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>fumari</rad>/o
</kap>
<drv mrk="fumari.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="papava.0oj">papavacoj</ref>
      (<trd lng="la">Fumaria</trd>).
    </dif>
    <ref tip="sin" cel="fum.ter0o">terfumo</ref>
  </snc>
  <trd lng="cs">dymnivka</trd>
  <trd lng="de">Erdrauch</trd>
  <trd lng="fr">fumeterre</trd>
  <trd lng="hu">f&uuml;stike</trd>
  <trd lng="nl">duivekerver</trd>
  <trd lng="pl">dymnica</trd>
  <trdgrp lng="pt">
    <trd>fum&aacute;ria</trd>,
    <trd>fumo-da-terra</trd>,
    <trd>erva-molarinha</trd>,
    <trd>fel-da-terra</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_y;&c_m;&c_ja;&c_n;&c_k;&c_a;</trd>
  <trd lng="sk">zemedym</trd>
</drv>
</art>
