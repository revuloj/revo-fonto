<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>fumari</rad>/o
</kap>
<drv mrk="fumari.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="fumari.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Fumaria</trd>).
    </dif>
    <ref tip="sin" cel="fum.ter0o">terfumo</ref>
  </snc>
  <trd lng="de">Erdrauch</trd>
</drv>
<drv mrk="fumari.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Fumariaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Erdrauchgew&auml;chse</trd>
</drv>
</art>
