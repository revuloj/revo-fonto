<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fumari.xml,v 1.24 2023/08/31 13:11:32 revo Exp $">
<kap>
<rad>fumari</rad>/o
</kap>
<drv mrk="fumari.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio 
      <ref tip="malprt" cel="papava.0oj">papavacoj</ref>
      (<trd lng="la">Fumaria</trd>):
      <ekz>
        <tld/>o estas <klr>[&#x2026;]</klr> kuracplanto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=20303">Fumario</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        sprono botanike estas plilongiga&jcirc;o de iaj floroj, ofte kornoforma kaj pinta, ekz.
        <klr>[&#x2026;]</klr> <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=20299">Sprono</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        fumarata acido trovi&gcirc;as en <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=521323">Fumarata acido</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="fum.ter0o">terfumo</ref>
    <bld lok="&WCU;/thumb/e/e4/Fumaria_officinalis_subsp._officinalis_sl2.jpg/640px-Fumaria_officinalis_subsp._officinalis_sl2.jpg" prm="CC BY-SA 4.0">ordinara <tld/>o (Fumaria officinalis), Falkenberg in Stammersdorf, Vieno
        <fnt>
          <aut>Stefan.lefnaer, propra verko</aut>
          <lok><url ref="&WCW;/File:Fumaria_officinalis_subsp._officinalis_sl2.jpg">&WCW;/File:Fumaria_officinalis_subsp._officinalis_sl2.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_y;&c_m;&c_n;&c_ib;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_j;&c_ch;&c_a;&c_ja; &c_r;&c_u;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">dymnivka</trd>
  <trd lng="de">Erdrauch</trd>
  <trd lng="fr">fumeterre</trd>
  <trd lng="hu">f&uuml;stike</trd>
  <trd lng="ja">&#x30AB;&#x30E9;&#x30AF;&#x30B5;&#x30B1;&#x30DE;&#x30F3;</trd>
  <trd lng="nl">duivekerver</trd>
  <trd lng="pl">dymnica</trd>
  <trdgrp lng="pt">
    <trd>fum&aacute;ria</trd>,
    <trd>fumo-da-terra</trd>,
    <trd>erva-molarinha</trd>,
    <trd>fel-da-terra</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_y;&c_m;&c_ja;&c_n;&c_k;&c_a;</trd>
  <trd lng="sk">zemedym</trd>
  <trd lng="uk">&c_r;&c_u;&c_t;&#x43A;&c_a;</trd>
</drv>
</art>
