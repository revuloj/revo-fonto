<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>hom</rad>/o
</kap>

<drv mrk="hom.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="hom.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <tezrad/>
    <dif>
      Genro el familio <ref tip="super" cel="homed.0oj"><tld/>edoj</ref>
      (<trd lng="la">Homo</trd>), el kiu plu nur vivas la specio
      <trd lng="la">Homo sapiens</trd>, plej evoluinta viva&jcirc;o de
      la tero, karakterizata per vertikala teni&gcirc;o, racia
      inteligenteco kaj parolkapablo:
      <ekz>
        <ind><tld/>oj</ind> ne &ccirc;iam racie agas;
        <trd lng="br">tud</trd>
        <trd lng="de">Leute</trd>
        <trd lng="en">people</trd>
        <trd lng="es">gente</trd>
        <trd lng="fr">gens</trd>
        <trd lng="hu">az <ind>emberek</ind></trd>
        <trd lng="pl">ludzie</trd>
        <trd lng="sk">&lcaron;udia</trd>
      </ekz>
      <ekz>
        &ccirc;iu <tld/>o amas sin mem
        <fnt><bib>F</bib>, <lok>&FE; 18</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="vir.0o">viro</ref>
    <refgrp tip="vid">
      <ref cel="antrop2.0o">antropologio</ref>,
      <ref cel="art.0o">arto</ref>,
      <ref cel="histor.0o">historio</ref>,
      <ref cel="medici.0o">medicino</ref>,
      <ref cel="mens.0o">menso</ref>,
      <ref cel="religi.0o">religio</ref>,
      <ref cel="scienc.0o.fako">scienco <sncref/></ref>,
      <ref cel="soci.0o.POL">socio <sncref/></ref>,
      <ref cel="tehxnik.0o">te&hcirc;niko</ref>
    </refgrp>
  </snc>
  <trdgrp lng="br">
    <trd>den</trd>,
    <trd>mab-den</trd>
  </trdgrp>
  <trd lng="cs">&ccaron;lov&ecaron;k</trd>
  <trd lng="de">Mensch</trd>
  <trd lng="el">
  &alfa_ton;&ny;&theta;&rho;&omega;&pi;&omikron;&sigma_fina;</trd>
  <trd lng="en">man</trd>
  <trdgrp lng="es">
    <trd>hombre<klr tip="ind"> (ser humano)</klr></trd>,
    <trd>humano</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>humain</ind></trd>,
    <trd>homme<klr tip="ind"> (&ecirc;tre humain)</klr></trd>,
    <trd>humain<klr tip="ind"> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&alef;&dalet;&fmem;</trd>,
    <trd>&bet;&fnun; &alef;&dalet;&fmem;</trd>,
    <trd>&bet;&fnun;
    <ind>&alef;&nun;&vav;&shin;</ind></trd>
  </trdgrp>
  <trd lng="hu">ember</trd>
  <trd lng="it">uomo</trd>
  <trd lng="ja">&#20154; [<ind>&#12402;&#12392;</ind>]</trd>
  <trd lng="nl">mens</trd>
  <trd lng="pl">cz&lstroke;owiek</trd>
  <trd lng="pt">homem<klr tip="ind"> (ser humando)</klr></trd>
  <trd lng="ru">&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>
  <trd lng="sk">&ccaron;lovek</trd>
  <trd lng="tp">jan</trd>
  <trd lng="vo">men</trd>
  <trd lng="zh">&#20154;<klr> [r&eacute;n]</klr></trd>
</drv>

<drv mrk="hom.0a">
  <kap><tld/>a</kap>
  <snc mrk="hom.0a.KOMUNE">
    <dif>
      De <tld/>o:
      <ekz>
        la <tld/>a parolo;
      </ekz>
      <ekz>
        <tld/>aj kadavroj;
      </ekz>
      <ekz>
        farita&jcirc;o de <tld/>aj manoj
        <fnt><bib>MT</bib>, <lok>&Psa; 135:15</lok></fnt>;
      </ekz>
      <ekz>
        la <ind>ne<tld/>aj</ind> krioj de lia edzino
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 12a</lok></fnt>.
        <trd lng="fr">inhumain</trd>
        <trd lng="ru">
&c_n;&c_je;&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_ch;&c_je;&c_s;&c_k;&c_i;
&c_j;
        </trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="br">
    <trd>-<ind>den</ind></trd>,
    <trd>denel</trd>
  </trdgrp>
  <trd lng="cs">lidsk&yacute;</trd>
  <trd lng="de">menschlich</trd>
  <trd lng="el">
  &alfa;&ny;&theta;&rho;&omega_ton;&pi;&jota;&ny;&omikron;&sigma_fina;
  </trd>
  <trd lng="es">humano</trd>
  <trd lng="fr">humain<klr tip="ind">
  (relatif &agrave; l'&ecirc;tre humain)</klr></trd>
  <trd lng="he">&alef;&nun;&vav;&shin;&yod;</trd>
  <trd lng="hu">emberi</trd>
  <trd lng="nl">menselijk</trd>
  <trdgrp lng="pl">
    <trd>cz&lstroke;owieczy</trd>,
    <trd>ludzki</trd>
  </trdgrp>
  <trd lng="pt">humano</trd>
  <trd lng="ru">
  &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;
  </trd>
  <trd lng="sk">&lcaron;udsk&yacute;</trd>
  <trd lng="tp">jan</trd>
  <trd lng="vo">menik</trd>
</drv>

<drv mrk="hom.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="hom.0aro.KOMUNE">
    <dif>
      La tuta <tld/>a gento:
      <ekz>
        grandegan signifon havus por la <tld/>aro la enkonduko de unu
        komune akceptita lingvo
        <fnt><bib>FK</bib>, <lok>6, artikoloj pri esperanto, el la unua
        libro de la lingvo esperanto</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="prt" cel="soci.0o.POL">socio</ref>
  <trd lng="br">denelezh</trd>
  <trd lng="de">Menschheit</trd>
  <trd lng="el">
  &alfa;&ny;&theta;&rho;&omega;&pi;&omikron_ton;&tau;&eta;&tau;&alfa;
  </trd>
  <trd lng="es">humanidad</trd>
  <trdgrp lng="fr">
    <trd>genre humain</trd>,
    <trd>humanit&eacute;<klr tip="ind"> (genre humain)</klr></trd>
  </trdgrp>
  <trd lng="he">&he;<ind>&alef;&nun;&vav;&shin;&vav;&tav;</ind></trd>
  <trd lng="hu">emberis&eacute;g</trd>
  <trd lng="nl">mensheid</trd>
  <trd lng="pl">ludzko&sacute;&cacute;</trd>
  <trd lng="pt">humanidade</trd>
  <trd lng="ru">
  &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;
  </trd>
  <trd lng="sk">&lcaron;udstvo</trd>
  <trd lng="vo">menad</trd>
  <trdgrp lng="zh">
    <trd>&#20154;&#31867;</trd>,
    <trd><klr>tr. </klr>&#20154;&#39006;<klr>
[r&eacute;nl&egrave;i]</klr></trd>
  </trdgrp>
</drv>

<drv mrk="hom.0aranismo">
  <kap><tld/>aranismo</kap>
  <snc mrk="hom.0aranismo.ESP">
    <uzo tip="fak">ESP</uzo>
    <dif>
      Doktrino postulanta, ke &ccirc;iu rigardu kaj amu &ccirc;iulandajn
      homojn kiel siajn fratojn.
    </dif>
  </snc>
  <trd lng="de">Humanismus</trd>
  <trd lng="fr">homaranisme</trd>
  <trd lng="hu">homaranizmus</trd>
  <trd lng="pl">hilelism</trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_o;&c_m;&c_a;&c_r;&c_a;&c_n;&c_i;&c_z;&c_m;</trd>,
    <trd>&c_g;&c_i;&c_l;&c_l;&c_je;&c_l;&c_i;&c_z;&c_m;</trd>
  </trdgrp>
  <trd lng="sk">humanizmus</trd>
</drv>

<drv mrk="hom.0ido">
  <kap><tld/>ido</kap>
  <snc mrk="hom.0ido.KOMUNE">
    <dif>
      Ido de <tld/>o:
      <ekz>
        ho, rememoru, kia estas la da&ubreve;ro de mia vivo, por kia
vanta&jcirc;o Vi
        kreis &ccirc;iujn <tld/>idojn
        <fnt><bib>MT</bib>, <lok>&Psa; 89:47</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="infan.0o">infano</ref>
  </snc>
  <trd lng="br">bugel</trd>
  <trdgrp lng="de">
    <trd>Menschenkind</trd>,
    <trd>Kind</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>enfant</trd>,
    <trd>fils d'homme</trd>
  </trdgrp>
  <trd lng="hu">emberfia</trd>
  <trd lng="nl">mensenkind</trd>
  <trd lng="pl">dziecko</trd>
  <trd lng="ru">&c_r;&c_je;&c_b;&c_jo;&c_n;&c_o;&c_k;</trd>
</drv>

<drv mrk="hom.0ino">
  <kap><tld/>ino<fnt>Z</fnt></kap>
  <snc mrk="hom.0ino.KOMUNE">
    <dif>
      Ina <tld/>o.
    </dif>
    <ref tip="sin" cel="vir.0ino">virino</ref>
  </snc>
  <trd lng="br">maouez</trd>
  <trd lng="de">Frau</trd>
  <trd lng="el">&gamma;&ypsilon;&ny;&alfa;&jota_ton;&kappa;&alfa;</trd>
  <trd lng="fr">femme<klr tip="ind"> (femelle de l'homme)</klr></trd>
  <trd lng="he">&bet;&tav; <ind>&alef;&nun;&vav;&shin;</ind></trd>
  <trd lng="hu">asszony</trd>
  <trd lng="nl">vrouw</trd>
  <trd lng="pl">kobieta</trd>
  <trd lng="pt">mulher</trd>
  <trd lng="ru">&c_zh;&c_je;&c_n;&c_shch;&c_i;&c_n;&c_a;</trd>
</drv>

<drv mrk="hom.cxiu0a">
  <kap>&ccirc;iu<tld/>a</kap>
  <snc mrk="hom.cxiu0a.KOMUNE">
    <ref tip="dif" cel="univer1.0a">universala</ref>
  </snc>
  <trd lng="el">
  &pi;&alfa;&gamma;&kappa;&omikron_ton;&sigma;&my;&jota;&omikron;
&sigma_fina;
  </trd>
  <trd lng="fr">universel</trd>
  <trd lng="hu">emberi<klr tip="amb"> (egyetemes)</klr></trd>
  <trd lng="pl">og&oacute;lnoludzki</trd>
  <trd lng="pt">universal</trd>
  <trd lng="ru">
  &c_o;&c_b;&c_shch;&c_je;&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_ch;&c_je;
&c_s;&c_k;&c_i;&c_j;
  </trd>
</drv>

<drv mrk="hom.0ama">
  <kap><tld/>ama</kap>
  <snc mrk="hom.0ama.KOMUNE">
    <dif>
      Zorganta pri la feli&ccirc;o de l' <tld/>oj.
    </dif>
    <ref tip="vid" cel="human.0a">humana</ref>
  </snc>
  <trd lng="br">dengarour</trd>
  <trd lng="es">fil&aacute;ntropo</trd>
  <trd lng="fr">philanthrope<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="he">&alef;&vav;&he;&bet; &bet;&resh;&yod;&vav;&tav;</trd>
  <trd lng="hu">emberszeret&odblac;</trd>
  <trd lng="nl">menslievend</trd>
  <trd lng="ru">
  &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;&c_o;&c_l;&c_ju;&c_b;&c_i;&c_v;
&c_y;&c_j;
  </trd>
</drv>

<drv mrk="hom.0evitulo">
  <kap><tld/>evitulo</kap>
  <snc mrk="hom.0evitulo.KOMUNE">
    <dif>
      <tld lit="H"/>o, kiu mal&scirc;atas kaj evitas aliajn <tld/>ojn.
    </dif>
  </snc>
  <trd lng="br">dengasaour</trd>
  <trd lng="el">
  &my;&jota;&sigma;&alfa_ton;&ny;&theta;&rho;&omega;&pi;&omikron;
&sigma_fina;
  </trd>
  <trd lng="es">mis&aacute;ntropo</trd>
  <trd lng="fr">misanthrope<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="he">&shin;&vav;&nun;&alef; &alef;&dalet;&fmem;</trd>
  <trd lng="hu">emberker&uuml;l&odblac;</trd>
  <trd lng="nl">misantroop</trd>
  <trdgrp lng="pl">
    <trd>mizantrop</trd>,
    <trd>odludek</trd>
  </trdgrp>
  <trd lng="pt">misantropo</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_l;&c_ju;&c_d;&c_i;&c_m;</trd>,
    <trd>&c_b;&c_i;&c_r;&c_ju;&c_k;</trd>,
    <trd>&c_m;&c_i;&c_z;&c_a;&c_n;&c_t;&c_r;&c_o;&c_p;</trd>
  </trdgrp>
</drv>

<drv mrk="hom.mult0a">
  <kap>mult<tld/>a, <var><kap>multe<tld/>a</kap></var>,
  <var><kap><tld/>plena</kap></var></kap>
  <snc mrk="hom.mult0a.KOMUNE">
    <dif>
      Enhavanta multajn <tld/>ojn; lo&gcirc;ata de multaj <tld/>oj:
      <ekz>
        <tld/>plena strato;
      </ekz>
      <ekz>
        mult<tld/>a lando;
      </ekz>
      <ekz>
        li ekrigardis tra la vitro sur la multe<tld/>an kaj multemovan
        straton
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">poblek</trd>
  <trdgrp lng="fr">
    <trd>anim&eacute;<klr tip="ind"> (populeux)</klr></trd>,
    <trd>populeux</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>n&eacute;pes</trd>,
    <trd>zs&uacute;folt</trd>
  </trdgrp>
  <trd lng="nl">dichtbevolkt</trd>
  <trdgrp lng="pl">
    <trd>ludny</trd>, <trd>pe&lstroke;en ludzi</trd>
  </trdgrp>
  <trd
lng="ru">&c_m;&c_n;&c_o;&c_g;&c_o;&c_l;&c_ju;&c_d;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="hom.kavern0o">
  <kap>kavern<tld/>o</kap>
  <snc mrk="hom.kavern0o.PRA">
    <uzo tip="fak">PRA</uzo>
    <dif>
      Pra<tld/>o, kiu lo&gcirc;is en kavernoj.
    </dif>
  </snc>
  <rim mrk="hom.kavern0o.RIM">
    Prahistoriistoj unue opiniis, ke homoj en iu epoko normale
lo&gcirc;is en
    kavernoj. Tiu nocio ne validas plu en moderna prahistorio.
    <aut>&jcirc;v, 2005-08-26</aut>
  </rim>
  <trd lng="de">H&ouml;hlenmensch</trd>
  <trd lng="es">cavern&iacute;cola</trd>
  <trd lng="fr">homme des cavernes</trd>
  <trd lng="hu">barlanglak&oacute;</trd>
  <trd lng="nl">holbewoner</trd>
  <trd lng="pl">jaskiniowiec</trd>
  <trd lng="pt">homem das cavernas</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_o;&c_g;&c_l;&c_o;&c_d;&c_i;&c_t;</trd>,
    <trd>&c_p;&c_je;&c_shch;&c_je;&c_r;&c_n;&c_y;&c_j;
&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">pra&ccaron;lovek</trd>
</drv>

<drv mrk="hom.komun0a">
  <kap>komun<tld/>a, <var><kap>komune<tld/>a</kap></var></kap>
  <snc mrk="hom.komun0a.KOMUNE">
    <dif>
      Interesanta &ccirc;iujn <tld/>ojn:
      <ekz>
        &ccirc;iu societo a&ubreve; kongreso aparte decidu ion tian, kio
        alproksimigus la grandan komune<tld/>an celon almena&ubreve; je
        unu pa&scirc;o
        <fnt><bib>EE</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>k&ouml;z&ouml;s emberi</trd>,
    <trd>&aacute;ltal&aacute;nos emberi</trd>
  </trdgrp>
  <trd lng="fr">d'<ind>int&eacute;r&ecirc;t</ind>
g&eacute;n&eacute;ral</trd>
  <trdgrp lng="pl">
    <trd>wsp&oacute;lny wszystkim ludziom</trd>,
    <trd>og&oacute;lnoludzki</trd>
  </trdgrp>
  <trd lng="ru">
  &c_o;&c_b;&c_shch;&c_je;&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_ch;&c_je;
&c_s;&c_k;&c_i;&c_j;
  </trd>
</drv>

<drv mrk="hom.negx0o">
  <kap>ne&gcirc;<tld/>o,
    <var><kap>ne&gcirc;viro</kap></var>
  </kap>
  <snc mrk="hom.negx0o.LUD">
    <dif>
      Skulptita figuro simila al homo farita el ne&gcirc;o.
    </dif>
  </snc>
  <trd lng="bg">&c_s;&c_n;&c_je;&c_zh;&c_je;&c_n; &c_ch;&c_o;&c_v;&c_je;&c_k;</trd>
  <trd lng="de">Schneemann</trd>
  <trd lng="en">snowman</trd>
</drv>

<drv mrk="hom.pajlo0o">
  <kap>pajlo<tld/>o</kap>
  <snc mrk="hom.pajlo0o.FRAZ">
    <uzo tip="stl">FRAZ</uzo>
    <dif>
      Homo, kiu nur &scirc;ajne funkcias, malanta&ubreve; kiu reale
decidas aliaj
      homoj ka&scirc;ataj:
      <ekz>
        hodia&ubreve;aj studoj tamen pruvas, ke li estis nura
pajlo<tld/>o: la
        vera &leftquot;Ido&rightquot; estis unu el la sekretarioj mem
        de la Komitato, nome Louis Couturat
        <fnt>
        <aut>T. Carlevaro</aut>,
        <vrk><url ref=
        "http://miresperanto.narod.ru/esperantologio/akademioj.htm">
        Mondlingvaj akademioj</url></vrk>,
        <lok>[vidita je 2006-06-08]</lok>
        </fnt>.
      </ekz>
      <ekz>
        temas pri piramideta strukturo kun pluraj pajlo<tld/>oj,
        <klr>...</klr> surscenigantaj simplan farson
        <fnt>
        <aut>J. Camacho</aut>,
        <vrk><url ref="http://www.rano.org/foirismo.html">La liturgio de
        l' foiro</url></vrk>,
        <lok>1999-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">homme de paille</trd>
  <trdgrp lng="pl">
    <trd>pozorant</trd>,
    <trd>przykrywka</trd>
  </trdgrp>
</drv>

<drv mrk="hom.pra0o">
  <kap>pra<tld/>o</kap>
  <snc mrk="hom.pra0o.PRA">
    <uzo tip="fak">PRA</uzo>
    <dif>
      Iu el la diversaj prahistoriaj, tre antikvaj homoj, anka&ubreve;
tiuj, kiuj
      rase malsimilas al nuntempuloj:
      <ekz>
        la&ubreve; via logiko, vortoj ne estas utilaj, &ccirc;ar
pra<tld/>oj fartis
        bone sene
        <fnt>
        <aut>J. Cook</aut>
        <vrk>Re: Demando por &ccirc;iuj...</vrk>
        <lok>soc.culture.esperanto, 1997-05-27</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Vormensch</trd>,
    <trd>Urmensch</trd>
  </trdgrp>
  <trd lng="fr">homme pr&eacute;historique</trd>
  <trdgrp lng="pl">
    <trd>pracz&lstroke;owiek</trd>,
    <trd><ind>cz&lstroke;owiek</ind> pierwotny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_r;&c_je;&c_v;&c_n;&c_i;&c_j;
<ind>&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</ind></trd>,
<trd>
&c_d;&c_o;&c_i;&c_s;&c_t;&c_o;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;
<ind>&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="hom.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="hom.sen0a.KOMUNE">
    <dif>
      Kie <tld/>oj ne estas, nevizitata, nelo&gcirc;ata:
      <ekz>
        la vojo inter la &gcirc;ardenoj estas sen<tld/>a
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 26a, p.
273a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">didud</trd>
  <trd lng="de">menschenleer</trd>
  <trd lng="es">deshabitado</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;sert<klr tip="ind"> (inoccup&eacute;)</klr></trd>,
    <trd>d&eacute;sertique<klr tip="ind"> (inoccup&eacute;)</klr></trd>,
    <trd>inhabit&eacute;</trd>,
    <trd>inoccup&eacute;</trd>,
    <trd>vide<klr tip="ind"> (inhabit&eacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>n&eacute;ptelen</trd>,
    <trd>kihalt</trd>,
    <trd>lakatlan</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>onbewoond</trd>,
    <trd>onbemand</trd>
  </trdgrp>
  <trd lng="pl">bezludny</trd>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_l;&c_ju;&c_d;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="hom.sen0ejo">
  <kap>sen<tld/>ejo</kap>
  <snc mrk="hom.sen0ejo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Loko, regiono, kie neniu homo vivas konstante:
      <ekz>
        la arbaraj sen<tld/>ejoj de la interna Afriko
        <fnt>
        <bib>Fab3</bib>, <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>
      </ekz>
      ;
      <ekz>
        li alvenis la regionon de la kristanoj trans la sen<tld/>ejo,
        tra kiu la&ubreve; homa memoro neniu andaluza armeo pasis
        <fnt>
        <aut>F. G. Bengtsson, trad. B. Nilsson</aut>,
        <vrk><url ref="http://www.luin.se/inko/010-4.pdf">Orm la
        Ru&gcirc;a</url></vrk>,
        <lok>&ccirc;apitro 7a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">d&eacute;sert<klr> (lieu inhabit&eacute;)</klr></trd>
  <trdgrp lng="pl">
    <trd>bezludzie</trd>,
    <trd>odludzie</trd>
  </trdgrp>
</drv>

<drv mrk="hom.sen0igi">
  <kap>sen<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="hom.sen0igi.KOMUNE">
    <dif>
      Senigi je lo&gcirc;antoj:
      <ekz>
        pesto kaj milito sen<tld/>igis la tutan landon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dezert.0igi.KOMUNE">dezertigi</ref>
    </refgrp>
  </snc>
  <trd lng="br">diduda&ntilde;<klr tip="ind"> (v.k.)</klr></trd>
  <trd lng="de">entv&ouml;lkern</trd>
  <trd lng="es">deshabitar</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;peupler</trd>,
    <trd>vider<klr tip="ind"> (de sa population)</klr></trd>
  </trdgrp>
  <trd lng="hu">eln&eacute;ptelen&iacute;t</trd>
  <trdgrp lng="nl">
    <trd>uitroeien</trd>, <trd>ontvolken</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>eksterminowa&cacute;</trd>, <trd>wyludni&cacute;</trd>
  </trdgrp>
  <trd lng="ru">
  &c_o;&c_b;&c_je;&c_z;&c_l;&c_ju;&c_d;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="hom.super0o">
  <kap>super<tld/>o</kap>
  <snc mrk="hom.super0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Imago de nova <tld/>tipo, havanta eksterordinare superajn
      kvalitojn.
    </dif>
    <rim>
      La nocio venas de Henricus M&uuml;ller kaj estis uzata poste de
      Herder, Goethe kaj Nietzsche. La nazioj misuzis la nocion por sia
      ideologio.
    </rim>
  </snc>
  <trd lng="br">dreistden</trd>
  <trd lng="de">&Uuml;bermensch</trd>
  <trd lng="fr">surhomme</trd>
  <trdgrp lng="hu">
    <trd>&uuml;bermensch</trd>,
    <trd>fels&odblac;bbrend&udblac; <ind>ember</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>&uuml;bermensch</trd>, <trd>superman</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>nadcz&lstroke;owiek</trd>, <trd>superman</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>
    &c_s;&c_v;&c_je;&c_r;&c_h;&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;
    </trd>,
    <trd>&c_s;&c_u;&c_p;&c_je;&c_r;&c_m;&c_je;&c_n;</trd>
  </trdgrp>
  <trd lng="sk">nad&ccaron;lovek</trd>
</drv>

<drv mrk="hom.super0a">
  <kap>super<tld/>a</kap>
  <snc mrk="hom.super0a.KOMUNE">
    <dif>
      Superanta la kapablojn de normala <tld/>o:
      <ekz>
        la dioj donis al li super<tld/>an memoron kaj la miraklan
        penetreman okulon
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 2a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">dreistdenel</trd>
  <trd lng="de">&uuml;bermenschlich</trd>
  <trd lng="fr">surhumain</trd>
  <trd lng="he">
  &ayin;&lamed;-<ind>&alef;&nun;&vav;&shin;&yod;</ind></trd>
  <trd lng="hu">emberfeletti</trd>
  <trd lng="pl">nadludzki</trd>
  <trd lng="ru">
 &c_s;&c_v;&c_je;&c_r;&c_h;&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_ch;&c_je;
&c_s;&c_k;&c_i;&c_j;
  </trd>
  <trd lng="sk">nad&lcaron;udsk&yacute;</trd>
</drv>
