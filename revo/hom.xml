      <ref cel="mens.0o">menso</ref>,
      <ref cel="religi.0o">religio</ref>,
      <ref cel="scienc.0o.fako">scienco <sncref/></ref>,
      <ref cel="soci.0o.POL">socio <sncref/></ref>,
      <ref cel="tehxnik.0o">te&hcirc;niko</ref>
    </refgrp>
    <trdgrp lng="th">
      <trd>&#xE04;&#xE19;</trd>,
      <trd>&#xE21;&#xE19;&#xE38;&#xE29;&#xE22;&#xE4C;</trd>
    </trdgrp>
  </snc>
  <trd lng="af">menslike</trd>
  <trd lng="am">&#x1230;&#x12CE;&#x127D;</trd>
  <trd lng="ar">&#x627;&#x644;&#x646;&#x627;&#x633;</trd>
