  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_r;&c_je;&c_v;&c_n;&c_i;&c_j;
    <ind>&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</ind></trd>,
    <trd>&c_d;&c_o;&c_i;&c_s;&c_t;&c_o;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;
    <ind>&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</ind></trd>
  </trdgrp>
  <trd lng="sk">pra&ccaron;lovek</trd>
  <trd lng="tr">tarih&ouml;ncesi insan&inodot;</trd>
</drv>

<drv mrk="hom.sen0a">
