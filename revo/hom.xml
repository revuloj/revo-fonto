<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>hom</rad>/o
</kap>
<drv mrk="hom.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="homed.0oj"><tld/>edoj</ref>
      (<trd lng="la">Homo</trd>), el kiu plu nur vivas la specio
      <trd lng="la">Homo sapiens</trd>, plej evoluinta viva&jcirc;o de la
      tero, karakterizata per vertikala teni&gcirc;o, racia
      inteligenteco kaj parolkapablo:
      <ekz>
        <ind><tld/>oj</ind> ne &ccirc;iam racie agas.
        <trd lng="en">people</trd>
        <trd lng="fr">gens</trd>
        <trd lng="pl">ludzie</trd>
      </ekz>
    </dif>
    <ref tip="vid" cel="vir.0o">viro</ref>
  </snc>
  <trd lng="cs">&ccaron;lov&ecaron;kaj</trd>
  <trd lng="de">Mensch</trd>
  <trdgrp lng="fr">
    <trd>homme<klr tip="ind"> (&ecirc;tre humain)</klr></trd>,
    <trd>humain<klr tip="ind"> (subst.)</klr></trd>,
    <trd>&ecirc;tre <ind>humain</ind></trd>
  </trdgrp>
  <trd lng="en">man</trd>
  <trd lng="es">hombre</trd>
  <trd lng="it">uomo</trd>
  <trd lng="nl">mens</trd>
  <trd lng="pl">cz&lstroke;owiek</trd>
  <trd lng="ru">&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>
</drv>

<drv mrk="hom.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      De <tld/>o:
      <ekz>
        la <tld/>a parolo;
      </ekz>
      <ekz>
        <tld/>aj kadavroj;
      </ekz>
      <ekz>
        la <ind>ne<tld/>aj</ind> krioj de lia edzino
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 12</lok></fnt>.
        <trd lng="fr">inhumain</trd>
      </ekz>
    </dif>
    <ref tip="sub" cel="human.0a">humana</ref>
  </snc>
  <adm>
    Kial la referenco al "humana" havas tipon "sub"?? <aut>MB</aut>
  </adm>
  <trd lng="cs">lidsk&yacute;</trd>
  <trd lng="de">menschlich</trd>
  <trd lng="fr">humain<klr tip="ind"> (relatif &agrave; l'&ecirc;tre humain)</klr></trd>
  <trd lng="nl">menselijk</trd>
  <trdgrp lng="pl">
    <trd>cz&lstroke;owieczy</trd>, <trd>ludzki</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="hom.0aro">
  <kap><tld/>aro</kap>
  <snc>
    <dif>
      La tuta <tld/>a gento.
    </dif>
  </snc>
  <trd lng="de">Menschheit</trd>
  <trdgrp lng="fr">
    <trd>humanit&eacute;<klr tip="ind"> (genre humain)</klr></trd>,
    <trd>genre <ind>humain</ind></trd>
  </trdgrp>
  <trd lng="nl">mensheid</trd>
  <trd lng="pl">ludzko&sacute;&cacute;</trd>
  <trd lng="ru">&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>

<drv mrk="hom.0aranismo">
  <kap><tld/>aranismo</kap>
  <uzo tip="fak">ESP</uzo>
  <snc>
    <dif>
      Doktrino postulanta, ke &ccirc;iu rigardu kaj amu &ccirc;iulandajn
      homojn kiel siajn fratojn.
    </dif>
  </snc>
  <trd lng="fr">homaranisme</trd>
  <trd lng="pl">hilelism</trd>
</drv>

<drv mrk="hom.0ido">
  <kap><tld/>ido</kap>
  <snc>
    <dif>
      Ido de <tld/>o.
    </dif>
    <ref tip="sin" cel="infan.0o">infano</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Menschenkind</trd>,
    <trd>Kind</trd>
  </trdgrp>
  <trd lng="nl">mensenkind</trd>
  <trd lng="pl">dziecko</trd>
</drv>

<drv mrk="hom.0ino">
  <kap><tld/>ino<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Ino de <tld/>o.
    </dif>
    <ref tip="sin" cel="vir.0ino">virino</ref>
  </snc>
  <trd lng="de">Frau</trd>
  <trd lng="fr">femme<klr tip="ind"> (femelle de l'homme)</klr></trd>
  <trd lng="nl">vrouw</trd>
</drv>

<drv mrk="hom.cxiu0a">
  <kap>&ccirc;iu<tld/>a</kap>
  <snc>
    <ref tip="dif" cel="univer1.0a">universala</ref>
  </snc>
  <trd lng="pl">og&oacute;lnoludzki</trd>
  <trd lng="ru">&c_o;&c_b;&c_shch;&c_je;&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="hom.0ama">
  <kap><tld/>ama</kap>
  <snc>
    <dif>
      Zorganta pri la feli&ccirc;o de l' <tld/>oj.
    </dif>
  </snc>
  <trd lng="fr">philanthrope<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="nl">menslievend</trd>
  <trd lng="ru">&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;&c_o;&c_l;&c_ju;&c_b;&c_i;&c_v;&c_y;&c_j;</trd>
</drv>

<drv mrk="hom.0evitulo">
  <kap><tld/>evitulo</kap>
  <snc>
    <dif>
      <tld lit="H"/>o, kiu mal&scirc;atas kaj evitas aliajn <tld/>ojn.
    </dif>
  </snc>
  <trd lng="fr">misanthrope<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="nl">misantroop</trd>
  <trdgrp lng="pl">
    <trd>mizantrop</trd>, <trd>odludek</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_l;&c_ju;&c_d;&c_i;&c_m;</trd>,
    <trd>&c_b;&c_i;&c_r;&c_ju;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="hom.0mangxulo">
  <kap><tld/>man&gcirc;ulo</kap>
  <snc>
    <ref tip="dif" cel="kaniba.0o">kanibalo</ref>
  </snc>
  <trd lng="de">Menschenfresser</trd>
  <trdgrp lng="fr">
    <trd>anthropophage<klr tip="ind"> (subst.)</klr></trd>,
    <trd>cannibale<klr tip="ind"> (subst.)</klr></trd>,
    <trd>mangeur d'hommes</trd>
  </trdgrp>
  <trd lng="nl">menseneter</trd>
  <trdgrp lng="pl">
    <trd>ludo&zdot;erca</trd>, <trd>kanibal</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_ju;&c_d;&c_o;&c_je;&c_d;</trd>
</drv>

<drv mrk="hom.0plenamult0a">
  <kap><tld/>plena, mult<tld/>a</kap>
  <snc>
    <dif>
      Enhavanta multajn <tld/>ojn; lo&gcirc;ata de multaj <tld/>oj:
      <ekz>
        <tld/>plena strato;
      </ekz>
      <ekz>
        mult<tld/>a lando.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>anim&eacute;<klr tip="ind"> (populeux)</klr></trd>,
    <trd>populeux</trd>
  </trdgrp>
  <trd lng="nl">dichtbevolkt</trd>
  <trdgrp lng="pl">
    <trd>ludny</trd>, <trd>pe&lstroke;en ludzi</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_n;&c_o;&c_g;&c_o;&c_l;&c_ju;&c_d;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="hom.kavern0o">
  <kap>kavern<tld/>o</kap>
  <uzo tip="fak">PRA</uzo>
  <snc>
    <dif>
      Praa <tld/>o, kiu lo&gcirc;is en kavernoj.
    </dif>
  </snc>
  <trd lng="de">H&ouml;hlenmensch</trd>
  <trd lng="fr">homme des cavernes</trd>
  <trd lng="nl">holbewoner</trd>
  <trd lng="pl">jaskiniowiec</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_o;&c_g;&c_l;&c_o;&c_d;&c_i;&c_t;</trd>,
    <trd>&c_p;&c_je;&c_shch;&c_je;&c_r;&c_n;&c_y;&c_j; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="hom.komun0a">
  <kap>komun<tld/>a, komune<tld/>a</kap>
  <snc>
    <dif>
      Interesanta &ccirc;iujn <tld/>ojn:
      <ekz>
        &ccirc;iu societo a&ubreve; kongreso aparte decidu ion tian, kio
        alproksimigus la grandan komune<tld/>an celon almena&ubreve; je unu
        pa&scirc;o
        <fnt><bib>EE</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="pl">
    <trd>wsp&oacute;lny wszystkim ludziom</trd>,
    <trd>og&oacute;lnoludzki</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_b;&c_shch;&c_je;&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="hom.sen0a">
  <kap>sen<tld/>a</kap>
  <snc>
    <dif>
      Nelo&gcirc;ata de <tld/>oj.
    </dif>
  </snc>
  <trd lng="de">menschenleer</trd>
  <trdgrp lng="fr">
    <trd>inhabit&eacute;</trd>,
    <trd>d&eacute;sert<klr tip="ind"> (inoccup&eacute;)</klr></trd>,
    <trd>d&eacute;sertique<klr tip="ind"> (inoccup&eacute;)</klr></trd>,
    <trd>inoccup&eacute;</trd>,
    <trd>vide<klr tip="ind"> (inhabit&eacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>onbewoond</trd>,
    <trd>onbemand</trd>
  </trdgrp>
  <trd lng="pl">bezludny</trd>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_l;&c_ju;&c_d;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="hom.sen0igi">
  <kap>sen<tld/>igi</kap>
  <snc>
    <dif>
      Senigi je lo&gcirc;antoj:
      <ekz>
        pesto kaj milito sen<tld/>igis la tutan landon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>d&eacute;peupler</trd>,
    <trd>vider<klr tip="ind"> (de sa population)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>uitroeien</trd>, <trd>ontvolken</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>eksterminowa&cacute;</trd>, <trd>wyludni&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_b;&c_je;&c_z;&c_l;&c_ju;&c_d;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="hom.super0o">
  <kap>super<tld/>o</kap>
  <uzo tip="fak">FIL</uzo>
  <snc>
    <dif>
      Imago de nova <tld/>tipo, havanta eksterordinare superajn kvalitojn.
    </dif>
    <rim>
      La nocio venas de Henricus M&uuml;ller kaj estis uzata poste de
      Herder, Goethe kaj Nietzsche. La nazioj misuzis la nocion por sia
      ideologio.
    </rim>
  </snc>
  <adm>
    Enkonduki "nazio", kiu aperas en la rimarko. <aut>MB></aut>
  </adm>
  <trd lng="de">&Uuml;bermensch</trd>
  <trd lng="fr">surhomme</trd>
  <trdgrp lng="nl">
    <trd>&uuml;bermensch</trd>, <trd>superman</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>nadcz&lstroke;owiek</trd>, <trd>superman</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_v;&c_je;&c_r;&c_h;&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>
</drv>

<drv mrk="hom.super0a">
  <kap>super<tld/>a</kap>
  <snc>
    <dif>
      Superanta la kapablojn de normala <tld/>o:
      <ekz>
        la dioj donis al li super<tld/>an memoron kaj la miraklan penetreman
        okulon
        <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 2</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">surhumain</trd>
</drv>
</art>
