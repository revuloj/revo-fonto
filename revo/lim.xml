      <ekz>
        sen<tld/>a multego
        <fnt>
          <bib>LR</bib>
          <lok>4. Vortaroj</lok>
        </fnt>;
      </ekz>
      <ekz>
        sen<tld/>a spaco
        <fnt>
          <bib>Fab3</bib>
          <lok>En la lasta tago</lok>
        </fnt>,
        tempo, povo, fiero, konfido, universo
        <fnt><bib>Fab4</bib> <lok>Du fratoj</lok></fnt>,
        &gcirc;ojo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>,
        profundega&jcirc;o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro 16a</lok>
        </fnt>;
      </ekz>
      <ekz>
        sen<tld/>e ami;
      </ekz>
      <ekz>
        li sentis en si sen<tld/>an kura&gcirc;on
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro 5a</lok>
        </fnt>;
      </ekz>
      <ekz>
        sen<tld/>a teruro lar&gcirc;igis &scirc;iajn pupilojn
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro 16a</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ekvidis la sen<tld/>an dezerton
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro 20a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
