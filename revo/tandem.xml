<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tandem.xml,v 1.29 2023/10/21 13:02:26 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>tandem</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tandem.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Transportilo kun du tirbestoj a&ubreve; pasageroj situantaj unu post
      alia:
    </dif>
    <subsnc mrk="tandem.0o.BIC">
      <dif>
        <ref tip="super" cel="bicikl.0o">Biciklo</ref> por du personoj,
        sidantaj unu malanta&ubreve; la alia.
      </dif>
      <trd lng="en">tandem bicycle</trd>
      <trd lng="es">t&aacute;ndem<klr> (bicicleta)</klr></trd>
      <trd lng="fr">tandem</trd>
      <trd lng="hu">tandemker&eacute;kp&aacute;r</trd>
      <trd lng="nl">tandem</trd>
      <trd lng="pl">tandem</trd>
    </subsnc>
    <subsnc mrk="tandem.0o.kalesxo">
      <dif>
        Malpeza <ref tip="super" cel="kalesx.0o">kale&scirc;o</ref> kun du
        &ccirc;evaloj, jungitaj unu
        malanta&ubreve; la alia.
      </dif>
      <trd lng="en">tandem</trd>
      <trd lng="hu">tandem</trd>
    </subsnc>
        <bld lok="&WCU;/thumb/5/57/Tandem_recumbent_bicycle.jpg/320px-Tandem_recumbent_bicycle.jpg" prm="CC-BY-SA-3.0"><tld/>a biciklo
        <fnt>
          <lok><url ref="&WCW;/File:Tandem_recumbent_bicycle.jpg">&WCW;/File:Tandem_recumbent_bicycle.jpg</url></lok>
        </fnt>
    </bld>
    <bld lok="&WCU;/thumb/7/77/Attelage_Tandem.JPG/320px-Attelage_Tandem.JPG" prm="CC-BY-SA-3.0"><tld/>a kale&scirc;o
        <fnt>
          <aut>Beideler Ren&eacute;</aut>
          <lok><url ref="&WCW;/File:Attelage_Tandem.JPG">&WCW;/File:Attelage_Tandem.JPG</url></lok>
        </fnt>
    </bld>
          <trd lng="be">&c_t;&c_a;&c_n;&c_d;&c_e;&c_m;</trd>
  </snc>
  <snc mrk="tandem.0o.arangxo">
     <dif>
       Tia aran&gcirc;o de ma&scirc;ino, ke du samspecaj partoj kunlaboras:
       <ekz>
         <tld/>a helikoptero <klr>(kun du samgrandaj helicoj)</klr>;
       </ekz>
       <ekz>
         <tld/> aviadilo <klr>(kun du samgranda flugilparoj)</klr>;
       </ekz>
       <ekz>
        sistemoj de <tld/>o uzas kelkajn sendependajn identajn
        procesorojn kaj redundajn
        memorunuojn
        <fnt>
          <vrk><url ref="https://epo.wikitrans.net/Tandem_Computers">Tandemaj Komputiloj - Wikipedia's Tandem Computers as
              ...</url></vrk>
        </fnt>;
       </ekz>
       <ekz>
         <tld/>a hidra&ubreve;<klr>[lik]</klr>cilindro
         <fnt>
           <bib>Viki</bib>,
           <vrk><url ref="&Viki;/Tandema_hidra%C5%ADcilindro">
           Tandema hidra&ubreve;cilindro</url></vrk>,
           <lok>2008-11-30</lok>
        </fnt>.
       </ekz>
     </dif>
     <ref tip="vid" cel="redund.0a">redunda</ref>
     <bld lok="&WCU;/thumb/7/76/Helicopter_-_Flickr_-_USDAgov.jpg/320px-Helicopter_-_Flickr_-_USDAgov.jpg" prm="PD"><tld/>a helikoptero
        <fnt>
          <aut>U.S. Department of Agriculture</aut>
          <lok><url ref="&WCW;/File:Helicopter_-_Flickr_-_USDAgov.jpg">&WCW;/File:Helicopter_-_Flickr_-_USDAgov.jpg</url></lok>
        </fnt>
     </bld>
     <trdgrp lng="be">
       <trd>&c_t;&c_a;&c_n;&c_d;&c_e;&c_m;</trd>,
       <trd>&c_s;&c_ch;&c_e;&c_p;</trd>
     </trdgrp>
     <trd lng="pl">tandem</trd>
   </snc>
   <snc mrk="tandem.0o.paro">
     <uzo tip="stl">FIG</uzo>
     <dif>
       Kunlaboranta <ref tip="dif" cel="par.0o">paro</ref>:
       <ekz>
         ankora&ubreve;foje la Monarkio kaj la Eklezio iras mane en mane
         <klr>[&#x2026;]</klr>, denove tiu-&ccirc;i <tld/>o vidas
         horore ke &leftquot;ilia Hispanio&rightquot;, ilia privata &ccirc;asejo,
         mar&scirc;<klr>as</klr> tra padoj de libereco
         <fnt>
           <aut>trad. Marteno</aut>,
           <vrk><url ref="http://satenhispanio.eresmas.com/artik10.htm">
           Denove la religio</url></vrk>,
           <lok>Sennaciulo, 2003-11</lok>
         </fnt>;
       </ekz>
       <ekz>
         <klr>[kiel]</klr> la plej intima kunlaboranto de Johano
         Pa&ubreve;lo la 2a, <klr>[&#x2026;]</klr>
         <nom>Ratzinger</nom> estis la teologo en tiu <tld/>o de la
         vatikana pinto
         <fnt>
           <aut>S. Maul</aut>,
           <vrk><url ref="&Monato;/2005/008684.php">
           Surprizo vatikana</url></vrk>,
           <lok>Monato, 2005</lok>
        </fnt>;
       </ekz>
       <ekz>
        la laboro en kundivido kondukas al tre homta&ubreve;gaj laborhoraroj
        kaj donas la
        eblecon senprobleme foje ne &ccirc;eesti, &ccirc;ar la partnero en la
        <tld/>o precize
        scias kio estas farenda
        <fnt>
          <vrk><url ref="https://www.lingq.com/en/learn-esperanto-online/courses/282910/kundividi-laborpostenon-1165514/">Esperanta Retradio 2014, Kundividi
              laborpostenon</url></vrk>
        </fnt>;
       </ekz>
       <ekz>
        lingvolerna inter&scirc;an&gcirc;o a&ubreve; <tld/>a lingvolernado estas metodo de
        lingvolernado
        bazita sur reciproka lingva inter&scirc;an&gcirc;o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=431759">Lingvolerna inter&scirc;an&gcirc;o</url></lok>
        </fnt>;
       </ekz>
       <ekz>
        drogkomercistoj kaj koruptaj policanoj laboras en <tld/>o por
        &ccirc;anta&gcirc;i grandegaj
        kvantoj de mono de naiva turistoj
        <fnt>
          <vrk><url ref="https://stonercircle.net/eo/2016/05/08/weed-in-thailand/">Turistoj Gvidi Por A&ccirc;etante Weed En Tajlando</url></vrk>
        </fnt>.
       </ekz>
     </dif>
     <ref tip="vid" cel="op.0o">duopo</ref>
     <trdgrp lng="be">
       <trd>&c_t;&c_a;&c_n;&c_d;&c_e;&c_m;</trd>,
       <trd>&c_p;&c_a;&c_r;&c_a;</trd>
     </trdgrp>
     <trd lng="pl">tandem</trd>
  </snc>
  <trd lng="ca">t&agrave;ndem</trd>
  <trd lng="cs">tandem</trd>
  <trd lng="de">Tandem</trd>
  <trd lng="ja">&#x30BF;&#x30F3;&#x30C7;&#x30E0;&#x8ECA;  <pr>&#x30BF;&#x30F3;&#x30C7;&#x30E0;&#x3057;&#x3083;</pr></trd>
  <trd lng="pl">tandem</trd><trd lng="pl">tandem</trd>
  <trdgrp lng="pt">
   <trd>tandem</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_n;&c_d;&c_je;&c_m;</trd>
  <trd lng="sk">&lcaron;ahk&yacute; voz s&nbsp;dvoma ko&ncaron;mi za sebou</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_a;&c_n;&c_d;&c_je;&c_m;</trd>,
    <trd>&c_d;&c_v;&c_o;&c_m;&c_ib;&c_s;&c_n;&c_i;&c_j; &c_v;&c_je;&c_l;&c_o;&c_s;&c_i;&c_p;&c_je;&c_d;</trd>
  </trdgrp>
</drv>
</art>
