<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>meduz</rad>/o
</kap>
<drv mrk="meduz.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="meduz.0o.ZOO">
    <dif>
      Libere na&gcirc;anta formo de
      <ref tip="malprt" cel="hidroz.0oj">hidrozooj</ref>.
    </dif>
    <ref tip="vid" cel="polip.0o">polipo</ref>
  </snc>
  <trd lng="de">Qualle</trd>
  <trd lng="es">medusa</trd>
  <trd lng="nl">kwal</trd>
  <trd lng="ru">&c_m;&c_je;&c_d;&c_u;&c_z;&c_a;</trd>
</drv>

<drv mrk="meduz.M0o">
  <kap>
    <tld lit="M"/>o
  </kap>
  <uzo tip="fak">MIT</uzo>
  <snc mrk="meduz.0o.MIT">
    <dif>
      Ina monstro de greka mitologio, kiu havis serpentojn anstata&ubreve;
      haroj kaj aliigis en &scirc;tonon &ccirc;iujn, kiuj &scirc;in rigardis.
    </dif>
  </snc>
  <trd lng="de">Medusa</trd>
  <trd lng="es">Medusa</trd>
  <trd lng="nl">Medusa</trd>
  <trd lng="ru">&c_M;&c_je;&c_d;&c_u;&c_z;&c_a;</trd>
</drv>

<drv mrk="meduz.grand0oj">
  <kap>
    grand<tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="hidroz.0oj">hidrozooj</ref>
      (<trd lng="la">Siphonophora</trd>).
    </dif>
  </snc>
  <trd lng="de">Staatsquallen</trd>
  <trd lng="ru">&c_s;&c_i;&c_f;&c_o;&c_n;&c_o;&c_f;&c_o;&c_r;&c_y; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
</drv>
</art>
