<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: meduz.xml,v 1.20 2020/06/27 20:32:48 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>meduz</rad>/o
</kap>
<drv mrk="meduz.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="meduz.0o.ZOO">
  <uzo tip="fak">ZOO</uzo>
    <dif>
      Libere na&gcirc;anta formo de
      <ref tip="malprt" cel="hidroz.0oj">hidrozooj</ref>:
      <ekz>
        kniduloj estas filumo kies membroj estas hidrozooj, maranemonoj,
        koraloj,
        <tld/>oj, ktp
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2429"
              >Knidulo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        marfruktoj kiel <tld/>oj, salikokoj, palinuroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=36992"
              >Fi&scirc;kapta industrio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="polip.0o">polipo</ref>
  </snc>                      
  <trd lng="be">&c_m;&c_e;&c_d;&c_u;&c_z;&c_a;</trd>
  <trd lng="br">morgaoulenn</trd>
  <trd lng="ca">medusa</trd>
  <trd lng="de">Qualle</trd>
  <trdgrp lng="en">
    <trd>jellyfish</trd>
    <trd>medusa</trd>
  </trdgrp>
  <trd lng="es">medusa</trd>
  <trd lng="fr">m&eacute;duse</trd>
  <trd lng="hu">med&uacute;za</trd>
  <trd lng="it">medusa</trd>
  <trd lng="nl">kwal</trd>
  <trd lng="pl">meduza</trd>
  <trd lng="pt">medusa</trd>
  <trd lng="ru">&c_m;&c_je;&c_d;&c_u;&c_z;&c_a;</trd>
</drv>

<drv mrk="meduz.M0o">
  <kap>
    <tld lit="M"/>o
  </kap>
  <snc mrk="meduz.0o.MIT">
  <uzo tip="fak">MIT</uzo>
    <dif>
      Ina mortema monstro de greka mitologio, unu el la 
      <ref tip="malprt" cel="gorgon.0o.MIT">gorgonoj</ref>, kiu havis
      serpentojn anstata&ubreve; haroj kaj aliigis en &scirc;tonon &ccirc;iujn,
      kiuj &scirc;in rigardis:
      <ekz>
        multaj soldatoj de Agenoro mortis fare de Perseo, sed ilia alta
        nombro faris lin
        malka&scirc;i denove la kapon de <tld lit="M"/>o kaj &scirc;tonigi Agenoron
        kaj liajn
        ceterajn apogantojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=115217"
              >Cefeo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>                      
  <trd lng="be">&c_M;&c_e;&c_d;&c_u;&c_z;&c_a;</trd>
  <trd lng="br">Meduza</trd>
  <trd lng="ca">Medusa</trd>
  <trd lng="de">Medusa</trd>
  <trd lng="en">Medusa</trd>
  <trd lng="es">Medusa</trd>  
  <trd lng="fr">M&eacute;duse</trd>
  <trd lng="hu">Med&uacute;za</trd>
  <trd lng="it">Medusa</trd>
  <trd lng="nl">Medusa</trd>
  <trd lng="pl">Meduza</trd>
  <trd lng="pt">Medusa</trd>
  <trd lng="ru">&c_M;&c_je;&c_d;&c_u;&c_z;&c_a;</trd>
</drv>

<drv mrk="meduz.grand0oj">
  <kap>
    grand<tld/>oj
  </kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      Ordo el la klaso <ref tip="super" cel="hidroz.0oj">hidrozooj</ref>
      (<trd lng="la">Siphonophora</trd>).
    </dif>
  </snc>
  <trd lng="ca">sifon&ograve;fors</trd>
  <trd lng="de">Staatsquallen</trd>
  <trd lng="en">Siphonophorae</trd>
  <trd lng="fr">siphonophores</trd>
  <trdgrp lng="hu">
    <trd>telepes <ind>med&uacute;z&aacute;k</ind></trd>,
    <trd>h&oacute;lyagos <ind>med&uacute;z&aacute;k</ind></trd></trdgrp>
  <trd lng="ru">&c_s;&c_i;&c_f;&c_o;&c_n;&c_o;&c_f;&c_o;&c_r;&c_y;
  <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trd lng="it">Sifonofori</trd>
</drv>
</art>
