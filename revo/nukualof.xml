<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: nukualof.xml,v 1.6 2023/09/03 06:36:23 afido Exp $">
    <kap>
      <rad>Nukualof</rad>/o
    </kap>

<drv mrk="nukualof.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      La <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&ccirc;efurbo</ref> de <ref tip="malprt" cel="tong.T0o.sxtato">Tongo</ref>:
      <ekz>
        la re&gcirc;a palaco kaj &ccirc;iuj konstrua&jcirc;oj de la registaro trovi&gcirc;as en <tld/>o
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Nukualofo">Nukualofo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la vulkano je 65 kilometroj for de <tld/>o, la &ccirc;efurbo de Tongo, havas du
        kraterojn, el kiuj unu estas sur senhoma insulo kaj la alia sub la marakvo
        kun la profundeco je 100 metroj
        <fnt>
          <aut>Xie Ruifeng</aut>
          <vrk><url ref="http://www.espero.com.cn/2015-01/19/content_34598128.htm">Nova insuleto
          aperis en Tongo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_N;&c_u;&c_k;&c_u;&c_a;&c_l;&c_o;&c_f;&c_a;</trd>
    <trd lng="de">Nuku&#x2BB;alofa</trd>
    <trd lng="en">Nuku&#x2BB;alofa</trd>
    <trd lng="fr">Nuku&#x2BB;alofa</trd>
    <trd lng="pl">Nuku&#x2BB;alofa</trd>
    <trd lng="ro">Nuku'alofa</trd>
  </snc>
  <trd lng="ja">&#x30CC;&#x30AF;&#x30A2;&#x30ED;&#x30D5;&#x30A1;</trd>
  <trd lng="uk">&#x41D;&c_u;&#x43A;&c_u;&c_a;&#x43B;&#x43E;&c_f;&c_a; (&c_s;&c_t;&#x43E;&#x43B;&c_i;&c_c;&#x44F;)</trd>
</drv>

  </art>

