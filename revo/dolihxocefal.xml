<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dolihxocefal.xml,v 1.14 2026/02/05 20:50:03 revo Exp $">
<kap>
  <rad>doli&hcirc;ocefal</rad>/o <fnt><bib>BoER</bib></fnt>,
  <var><kap><rad var="k">dolikocefal</rad>/o <fnt><bib>PV</bib></fnt></kap></var>
</kap>
<drv mrk="dolihxocefal.0o">
  <kap><tld/>o,
    <var><kap><tld var="k"/>o</kap></var>
  </kap>
  <snc mrk="dolihxocefal.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Homo, kies <ref cel="krani.0o">kranio</ref> estas pli longa je unu
      kvarono ol &gcirc;ia lar&gcirc;o, longkraniulo:
      <ekz>
        pli ol avera&gcirc;e <tld/>eco okazas &ccirc;e homoj kun <nom>Marfan</nom>-sindromo,
        <nom>Edwards</nom>-sindromo (trisomio 18),
        <nom>Prader-Willi</nom>-sindromo, <nom>Wolf-Hirschhorn</nom>-sindromo kaj N-sindromo;
      </ekz>
      <ekz>
        diversaj kulturoj konsideris <tld/>econ bela kaj misformis infanajn kraniojn por atingi
        tion.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="brahxicefal.0o">bra&hcirc;icefalo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_l;&c_ib;&c_h;&c_a;&c_c;&c_e;&c_f;&c_a;&c_l;</trd>,
    <trd>&c_d;&c_o;&c_w;&c_g;&c_a;&c_g;&c_a;&c_l;&c_o;&c_v;&c_y; <klr>(&c_n;&c_a;&c_z;.)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Langsch&auml;del</trd>,
    <trd>Dolichozephalus</trd>
  </trdgrp>
  <trd lng="en">dolichocephal</trd>
  <trd lng="fr">dolichoc&eacute;phale<klr> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>dolichocephal</trd>,
    <trd>dolichokephal</trd>,
    <trd>hossz&uacute;fej&udblac;</trd>
  </trdgrp>
  <trd lng="it">dolicocefalo</trd>
  <trd lng="ja">&#x9577;&#x982D;  <pr>&#x3061;&#x3087;&#x3046;&#x3068;&#x3046;</pr></trd>
  <trd lng="pl">d&lstroke;ugoczaszkowiec</trd>
  <trd lng="pt">dolicoc&eacute;falo</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_l;&c_i;&c_h;&c_o;&c_c;&c_je;&c_f;&c_a;&c_l;</trd>,
    <trd>&c_d;&c_l;&c_i;&c_n;&c_n;&c_o;&c_g;&c_o;&c_l;&c_o;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_o;&c_l;&c_ib;&c_h;&c_o;&c_c;&c_je;&c_f;&c_a;&c_l;</trd>,
    <trd>&c_d;&c_o;&c_v;&c_g;&c_o;&c_g;&c_o;&c_l;&c_o;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
