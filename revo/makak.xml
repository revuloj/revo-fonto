<art mrk="$Id: makak.xml,v 1.21 2022/05/01 12:05:12 revo Exp $">
<kap>
  <rad>makak</rad>/o
</kap>
<drv mrk="makak.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Mezgranda <ref tip="lst" cel="simi.0o" lst="voko:simioj">simio</ref>,
      kun fortaj brakoj kaj kruroj, grize bruna &gcirc;is preska&ubreve; nigra felo,
      senhara viza&gcirc;o kun muzelo, ofte kun mallonga vosto;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio
      <ref tip="malprt" cel="cerkop1.0oj">cerkopitekedoj</ref>
      (<trd lng="la">Macaca</trd>), kies plej multaj specioj vivas en
      Azio:
      <ekz>
        &ccirc;e aparta speco de afrikaj simioj, <tld/>oj, estis malkovritaj
        virusoj gene
        proksimaj al homa imundifektiga viruso de tipo 2 (HIV-2), kiu
        pli ofte
        renkonti&gcirc;as en okcidenta Afriko
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2004/008160.html">Afriko kaj aidoso</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/1/18/Juveniler_Japanmakak_(Macaca_fuscata)_im_Jigokudani_Yaen_K&#x14D;en.jpg/384px-Juveniler_Japanmakak_(Macaca_fuscata)_im_Jigokudani_Yaen_K&#x14D;en.jpg" prm="CC BY-SA 4.0">juna japana <tld/>o (Macaca fuscata) montoj de Nagano, Japanujo
        <fnt>
          <aut>&#xA9; Frank Schulenburg, propra verko</aut>
          <lok><url ref="&WCW;/File:Juveniler_Japanmakak_(Macaca_fuscata)_im_Jigokudani_Yaen_K%C5%8Den.jpg">&WCW;/File:Juveniler_Japanmakak_(Macaca_fuscata)_im_Jigokudani_Yaen_K&#x14D;en.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_k;&c_a;&c_k;&c_a;</trd>
  <trd lng="ca">macac</trd>
  <trd lng="de">Makake</trd>
  <trd lng="en">macaque</trd>
  <trd lng="fr">macaque</trd>
  <trd lng="hu">mak&aacute;k&oacute;</trd>
  <trd lng="id">makaka</trd>
  <trdgrp lng="nl">
    <trd>makaak</trd>,
    <trd>langstaartaap</trd>
  </trdgrp>
  <trd lng="pl">makak</trd>
  <trd lng="pt">macaca</trd>
  <trd lng="ro">macac</trd>
  <trd lng="ru">&c_m;&c_a;&c_k;&c_a;&c_k;&c_a;</trd>
  <trd lng="uk">&#x43C;&#x430;&#x43A;&#x430;&#x43A;&#x430;</trd>
</drv>
</art>
