<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: digita1.xml,v 1.15 2023/08/31 10:44:00 revo Exp $">
<kap>
  <rad>digitalin</rad>/o
</kap>
<drv mrk="digita1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      C<sub>35</sub>H<sub>56</sub>O<sub>14</sub>; venena alkaloido,
      ekstraktebla el <ref tip="malprt" cel="digita.0o">digitalo</ref>:
      <ekz>
        glikozidoj kemie ili estas kombina&jcirc;o inter ia karbonhidrato kun ia alkoholo, kaj
        oni distingas diversajn subgrupojn el ili: flavonoidoj, kor-glikozidoj
        (<tld/>o, digoksino kaj aliaj), fenol-glikozidoj kaj saponinoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18114">Kuracplanto</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_g;&c_ib;&c_t;&c_a;&c_l;&c_ib;&c_n;</trd>
  <trd lng="ca">digitalina</trd>
  <trdgrp lng="de">
     <trd>Digitalin</trd>,
     <trd>Digitoxin</trd>
  </trdgrp>
  <trd lng="es">digitalina</trd>
  <trd lng="fr">digitaline</trd>
  <trd lng="he">&#x5D0;&#x5E6;&#x5D1;&#x5E2;&#x5D5;&#x5E0;&#x5D9;&#x5EA;</trd>
  <trd lng="hu">digitalin</trd>
  <trd lng="ja">&#x30B8;&#x30AE;&#x30BF;&#x30EA;&#x30F3;</trd>
  <trd lng="nl">digitaline</trd>
  <trd lng="pt">digitalina</trd>
  <trd lng="ru">&c_d;&c_i;&c_g;&c_i;&c_t;&c_a;&c_l;&c_i;&c_n;</trd>
</drv>
</art>
