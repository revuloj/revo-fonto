  <trd lng="hy">&#x574;&#x56B;&#x57B;&#x565;&#x582;</trd>
  <trd lng="is">milli</trd>
  <trd lng="ja">&#x9593;&#x306B;</trd>
  <trd lng="jw">antarane</trd>
  <trd lng="ka">&#x10E8;&#x10DD;&#x10E0;&#x10D8;&#x10E1;</trd>
