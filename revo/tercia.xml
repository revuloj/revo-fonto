<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <rad>terciar</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tercia.0a">
   <kap><tld/>a</kap>
   <snc mrk="tercia.0a.GEOL">
     <uzo tip="fak">GEOL</uzo>
     <uzo tip="klr">(pri epoko)</uzo>
     <dif>
       Apartenanta al la tria epoko.
     </dif>
     <refgrp tip="vid">
       <ref cel="kvater.0a">kvaternara</ref>,
       <ref cel="primar.0a">primara</ref>,
       <ref cel="sekund2.0a">sekundara</ref>.
     </refgrp>
   </snc>
   <trd lng="be">&c_t;&c_r;&c_a;&c_c;&c_ib;&c_ch;&c_n;&c_y;</trd>
   <trd lng="ca">terciari</trd>
   <trd lng="de">terti&auml;r</trd>
   <trd lng="fr">tertiaire<klr> (adj.)</klr></trd>
   <trd lng="hu">harmadkori</trd>
   <trd lng="nl">tertiair</trd>
   <trdgrp lng="pl">
     <trd>trzeciorz&eogonek;dowy</trd>,
     <trd>kenozoiczny</trd>
   </trdgrp>  
   <trdgrp lng="pt">
    <trd>terci&aacute;rio</trd>
   </trdgrp>  
   <trd lng="ru">&c_t;&c_r;&c_je;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
