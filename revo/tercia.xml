<vortaro>
<art mrk="$Id: tercia.xml,v 1.28 2025/09/28 08:00:22 revo Exp $">
<kap>
   <rad>terciar</rad>/a <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="tercia.0a">
  <kap><tld/>a</kap>
  <snc mrk="tercia.0a.SCI">
    <dif>
      Tria, triaranga, triagrada.
    </dif>
    <subsnc mrk="tercia.0a.GEOL">
      <uzo tip="fak">GEOL</uzo>
      <uzo tip="klr">(pri epoko)</uzo>
      <dif>
        Apartenanta al la tria epoko.
      </dif>
    </subsnc>
    <subsnc mrk="tercia.0a.EKON">
      <uzo tip="fak">EKON</uzo>
      <dif>
        Kiu provizas servojn, nemateriajn venda&jcirc;ojn:
        <ekz>
          malgra&ubreve; la notinda kresko de la <tld/>a sektoro
          <klr>...</klr>, oni neniam produktis tiom da varoj
          <fnt>
            <aut>D. Kessous</aut>,
            <vrk><url ref="http://www.satesperanto.org/Rauxmismo-kaj-postmodernismo.html">
            Ra&ubreve;mismo kaj postmodernismo</url></vrk>,
            <lok>SAT Kulturo, [vidita en 2014]</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="primar.0a">primara</ref>,
      <ref cel="sekund2.0a">sekundara</ref>,
      <ref cel="kvater.0a">kvaternara</ref>
    </refgrp>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>triavica</ke>.
  </rim>

  <trd lng="be">&c_t;&c_r;&c_a;&c_c;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">terciari</trd>
  <trdgrp lng="cs">
    <trd>troj&iacute;ho druhu</trd>,
    <trd>t&rcaron;et&iacute;ho &rcaron;&aacute;du</trd>
  </trdgrp>
  <trd lng="de">terti&auml;r</trd>
  <trd lng="en">tertiary</trd>
  <trd lng="es">terciario</trd>
  <trd lng="fr">tertiaire<klr> (adj.)</klr></trd>
  <trd lng="hu">harmadkori</trd>
  <trdgrp lng="ja">
    <trd>&#31532;&#19977;&#12398;  <pr>&#12384;&#12356;&#12415;&#12387;&#12398;</pr></trd>,
    <trd>&#19977;&#27425;&#12398;  <pr>&#12415;&#12387;&#12376;&#12398;</pr></trd>,
    <trd>&#31532;&#19977;&#32000;&#12398;  <pr>&#12384;&#12356;&#12373;&#12435;&#12365;&#12398;</pr></trd>
  </trdgrp>
  <trd lng="nl">tertiair</trd>
  <trdgrp lng="pl">
    <trd>trzeciorz&eogonek;dowy</trd>,
    <trd>kenozoiczny</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>terci&aacute;rio</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">tretieho r&aacute;du</trd>
  <trd lng="uk">&c_t;&c_r;&c_je;&c_t;&c_i;&c_n;&c_n;&c_i;&c_j;</trd>
</drv>
</art>
