<vortaro>
<art mrk="$Id$">
<kap>
   <rad>terciar</rad>/a
</kap>
<drv mrk="tercia.0a">
   <kap><tld/>a</kap>
   <uzo tip="fak">MIN</uzo>
   <snc>
     <uzo tip="klr">(pri epoko)</uzo>
     <dif>
       Apartenanta al la tria epoko.
     </dif>
     <refgrp tip="vid">
       <ref cel="kvater.0a">kvaternara</ref>,
       <ref cel="primar.0a">primara</ref>,
       <ref cel="sekund2.0a">sekundara</ref>.
     </refgrp>
   </snc>
   <trd lng="be">&c_t;&c_r;&c_a;&c_c;&c_ib;&c_ch;&c_n;&c_y;</trd>
   <trd lng="de">terti&auml;r</trd>
   <trd lng="fr">tertiaire</trd>
   <trd lng="hu">harmadkori</trd>
   <trd lng="nl">tertiair</trd>
   <trdgrp lng="pt">
    <trd>terci&aacute;rio</trd>
   </trdgrp>  
   <trd lng="ru">&c_t;&c_r;&c_je;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
