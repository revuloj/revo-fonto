    <trd>hormat<klr> (v.)</klr></trd>,
    <trd><baz>hormat</baz>menghormati</trd>,
    <trd><baz>mulia</baz>memuliakan</trd>,
    <trd><baz>respek</baz>merespek</trd>,
    <trd><baz>takzim</baz>menakzimkan</trd>
  </trdgrp>
  <trdgrp lng="it">
