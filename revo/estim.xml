<drv mrk="estim.mem0o">
  <kap>mem<tld/>o</kap>
  <snc mrk="estim.mem0o.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      <tld lit="E"/>o, takso pri la propra valoro,
      <ref tip="sin" cel="dign.mem0o.PSI">memdigno</ref>,
      <ref tip="sin" cel="respek.mem0o.MOR">memrespekto</ref>:
      <ekz>
        li salutis la princon kun mem<tld/>o kaj, <klr>[&#8230;]</klr> diris mallongan benon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        tia amego flatis multege lian mem<tld/>on
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj manieroj estas simplaj, preska&ubreve; modestaj, sed ne sen mem<tld/>o
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XXVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        paroli la anglan helpas al la mem<tld/>o de homoj
        <fnt>
          <aut>M.G. Rison</aut>,
          <vrk>Pli bone la anglan ol la denaskan</vrk>,
          <lok><url ref="&SCE;">soc.culture.esperanto</url>,
          1996-02-19</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aplomb.0o">aplombo</ref>,
