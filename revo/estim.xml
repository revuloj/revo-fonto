<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: estim.xml,v 1.33 2021/04/27 07:03:40 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>estim</rad>/i <fnt><bib>PV</bib></fnt> 
</kap>
<drv mrk="estim.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="estim.0i.KOMUNE">
    <dif>
      Alte <ref tip="vid" cel="taks.0i.KOMUNE">taksi<sncref/></ref>
      ies moralan a&ubreve; spiritan valoron:
      <ekz>
        oni &scirc;atas la talenton, oni <tld/>as la
        honestecon<fnt>B</fnt>;
      </ekz>
      <ekz>
        se vi scius, kia li estas, vi lin pli <tld/>us;
      </ekz>
      <ekz>
        <tld/>i iun, la meriton de iu, la farojn de iu.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="admir.0i">admiri</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_v;&c_a;&c_zh;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_u;&c_v;&c_a;&c_zh;&c_a;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_c;&c_je;&c_n;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_ch;&c_i;&c_t;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ct&iacute;t</trd>,
    <trd>m&iacute;t koho v &uacute;ct&ecaron;</trd>,
    <trd>v&aacute;&zcaron;it si</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>achten</trd>,
    <trd>sehr <ind>sch&auml;tzen</ind></trd>,
    <trd>hochsch&auml;tzen</trd>,
    <trd>hochachten</trd>,
    <trd>hoch <ind>achten</ind></trd>,
    <trd>ehren<klr> [leterstilo]</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>esteem</trd>,
    <trd>regard</trd>,
    <trd>respect</trd>
  </trdgrp>
  <trd lng="es">estimar</trd>
  <trd lng="fr">estimer</trd>
  <trdgrp lng="hu">
    <trd>tisztel</trd>,
    <trd>becs&uuml;l</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>rispettare</trd>,
    <trd>stimare</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>achten</trd>,
    <trd>hoogachten</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>szanowa&cacute;</trd>,
    <trd>powa&zdot;a&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>estimar</trd>,
    <trd>avaliar</trd>,
    <trd>gostar de</trd>,
    <trd>prezar</trd>,
    <trd>apreciar</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_v;&c_a;&c_zh;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>cti&tcaron; si</trd>,
    <trd>preukazova&tcaron; &uacute;ctu</trd>,
    <trd>v&aacute;&zcaron;i&tcaron; si</trd>
  </trdgrp>
  <trd lng="vo">stim&ouml;n</trd>
</drv>

<drv mrk="estim.0inda">
  <kap><tld/>inda</kap>
  <snc mrk="estim.0inda.KOMUNE">
    <dif>
      Meritanta <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_v;&c_a;&c_zh;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_sh;&c_a;&c_n;&c_o;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_d;&c_o;&c_s;&c_t;&c_o;&c_je;&c_n; &c_z;&c_a; &c_u;&c_v;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="cs">&uacute;ctyhodn&yacute;</trd>
  <trdgrp lng="de">
    <trd>achtenswert</trd>,
    <trd>achtbar</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>estimable</trd>,
    <trd>respectable</trd>,
    <trd>worthy</trd>
  </trdgrp>
  <trd lng="es">estimable</trd>
  <trd lng="fr">estimable</trd>
  <trd lng="he">&#x5E8;&#x5D0;&#x5D5;&#x5D9; &#x5DC;&#x5D4;&#x5E2;&#x5E8;&#x5DB;&#x5D4;</trd>
  <trd lng="hu">tisztelend&odblac;</trd>
  <trd lng="nl">achtbaar</trd>
  <trdgrp lng="pl">
    <trd>szacowny</trd>,
    <trd>czcigodny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_s;&c_t;&c_o;&c_j;&c_n;&c_y;&c_j;
    &c_u;&c_v;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_ch;&c_t;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ctihodn&yacute;</trd>,
    <trd>&uacute;ctyhodn&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="estim.0o">
  <kap><tld/>o</kap>
  <snc mrk="estim.0o.KOMUNE">
    <dif>
      Ago <tld/>i kaj rezulto de tiu ago:
      <ekz>
        bona <tld/>o valoras pli ol oro kaj ar&gcirc;ento;
      </ekz>
      <ekz>
        lia merito altiris al li la <tld/>on de la kompetentuloj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_v;&c_a;&c_g;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_sh;&c_a;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_u;&c_v;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_ch;&c_i;&c_t;</trd>
  </trdgrp>
  <trd lng="cs">&uacute;cta</trd>
  <trdgrp lng="de">
    <trd>Wertsch&auml;tzung</trd>,
    <trd>Achtung</trd>,
    <trd>Hochachtung</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>esteem</trd>,
    <trd>regard</trd>,
    <trd>respect</trd>
  </trdgrp>
  <trd lng="es">estima</trd>
  <trd lng="fr">estime</trd>
  <trd lng="he">&#x5D4;&#x5E2;&#x5E8;&#x5DB;&#x5D4;</trd>
  <trdgrp lng="hu">
    <trd>tisztelet</trd>,
    <trd>megbecs&uuml;l&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>rispetto</trd>,
    <trd>stima</trd>
  </trdgrp>
  <trd lng="nl">achting</trd>
  <trdgrp lng="pl">
    <trd>szacunek</trd>,
    <trd>poszanowanie</trd>,
    <trd>powa&zdot;anie</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_v;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">&uacute;cta</trd>
  <trd lng="vo">stim</trd>
</drv>

<drv mrk="estim.mal0i">
  <kap><ofc>*</ofc> mal<tld/>i
  <fnt><bib>F</bib>, <lok>Universala Vortaro, mal'</lok></fnt></kap>
  <snc mrk="estim.mal0i.KOMUNE">
    <dif>
      Malalte taksi ies moralan a&ubreve; spiritan valoron:
      <ekz>
        sa&gcirc;on kaj instruon la malsa&gcirc;uloj
        mal<tld/>as<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi mal<tld/>as lin pro lia mensogemo;
      </ekz>
      <ekz>
        Vi faris nin mal<tld/>ata&jcirc;o por niaj najbaroj
        <fnt><bib>MT</bib> <lok>&Psa; 44:13</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_g;&c_a;&c_r;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_p;&c_r;&c_je;&c_z;&c_i;&c_r;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>nen&aacute;vid&ecaron;t</trd>,
    <trd>opovrhovat</trd>,
    <trd>pohrdat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gering sch&auml;tzen</trd>,
    <trd>verachten</trd>,
    <trd>missachten</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>disdain</trd>,
    <trd>hold in contempt</trd>,
    <trd>disrespect</trd>,
    <trd>despise</trd>
  </trdgrp>
  <trd lng="es">desestimar</trd>
  <trd lng="fr">m&eacute;priser</trd>
  <trd lng="he">&#x5DC;&#x5D1;&#x5D5;&#x5D6;</trd>
  <trdgrp lng="hu">
    <trd>megvet</trd>,
    <trd>len&eacute;z</trd>,
  </trdgrp>
  <trdgrp lng="it">
    <trd>disprezzare</trd>,
    <trd>disdegnare</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verachten</trd>,
    <trd>minachten</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>gardzi&cacute;</trd>,
    <trd>pogardza&cacute;</trd>,
    <trd>wzgardza&cacute;</trd>,
    <trd>lekcewa&zdot;y&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_z;&c_i;&c_r;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>opovrhova&tcaron;</trd>,
    <trd>poh&racute;da&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="estim.mal0igi">
  <kap>mal<tld/>igi</kap>
  <snc mrk="estim.mal0igi.KOMUNE">
    <dif>
    Igi iun malestimata :
     <ekz>
      ...malspritan petola&jcirc;on, kiu havus kiel solan rezultaton min
      mal<tld/>igi kaj cin ridindigi de &ccirc;iuj homoj, kiuj nin konas?
      <fnt>
        <aut>H. Vallienne</aut>
        <vrk><url
        ref="http://tekstaro.com/pl/prenitutantekston.pl?nomo=chu-li&amp;tipo=&amp;sekcio=chuli-9&amp;antauasekcio=1&amp;postasekcio=1">&Ccirc;u
        li?</url></vrk>
        <lok>&Ccirc;apitro 9</lok>
      </fnt>
     </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>rendre m&eacute;prisable</trd>,
    <trd>abaisser</trd>
  </trdgrp>
</drv>  

<drv mrk="estim.mal0inda">
  <kap>mal<tld/>inda</kap>
  <snc mrk="estim.mal0inda.KOMUNE">
    <dif>
      Ne inda je <tld/>o:
      <ekz>
        kiel mal<tld/>inda vi fari&gcirc;is,
        &scirc;an&gcirc;ante vian vojon!
        <fnt><bib>MT</bib>, <lok>&Jer; 2:26</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ib;&c_k;&c_ch;&c_e;&c_m;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_v;&c_a;&c_r;&c_t;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_g;&c_o;&c_d;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">opovr&zcaron;en&iacute;hodn&yacute;</trd>
  <trd lng="fr">m&eacute;prisable</trd>
  <trd lng="nl">verachtelijk</trd>
  <trd lng="sk">opovrhnutia hodn&yacute;</trd>
</drv>

<drv mrk="estim.mal0o">
  <kap>mal<tld/>o</kap>
  <snc mrk="estim.mal0o.KOMUNE">
    <dif>
      Malalta takso; sento, ke io a&ubreve; iu
      estas malgrandvalora:
      <ekz>
        malri&ccirc;eco ne estas krimo, tamen kondukas al
        mal<tld/>o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_g;&c_a;&c_r;&c_d;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_g;&c_a;&c_r;&c_d;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_r;&c_je;&c_z;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>opovr&zcaron;en&iacute;</trd>,
    <trd>pohrd&aacute;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>contempt</trd>,
    <trd>disdain</trd>
  </trdgrp>
  <trd lng="fr">m&eacute;pris</trd>
  <trdgrp lng="it">
    <trd>disprezzo</trd>,
    <trd>disdegno</trd>
  </trdgrp>
  <trd lng="nl">minachting</trd>
  <trdgrp lng="sk">
    <trd>opovrhnutie</trd>,
    <trd>poh&racute;danie</trd>
  </trdgrp>
</drv>

<drv mrk="estim.mem0o">
  <kap>mem<tld/>o</kap>
  <snc mrk="estim.mem0o.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      <tld lit="E"/>o, takso pri la propra valoro,
      <ref tip="sin" cel="dign.mem0o.PSI">memdigno</ref>,
      <ref tip="sin" cel="respek.mem0o.MOR">memrespekto</ref>:
      <ekz>
        paroli la anglan helpas al la mem<tld/>o de homoj
        <fnt>
        <aut>M.G. Rison</aut>,
        <vrk>Pli bone la anglan ol la denaskan</vrk>,
        <lok><url
        ref="http://groups.google.be/group/soc.culture.esperanto?hl=eo">soc.culture.esperanto</url>,
        1996-02-19</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="am.mem0o.PSI">memamo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_m;&c_a;&c_p;&c_a;&c_v;&c_a;&c_g;&c_a;</trd>
  <trd lng="bg">&c_s;&c_a;&c_m;&c_o;&c_u;&c_v;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>d&uring;stojnost</trd>,
    <trd>sebe&uacute;cta</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>self esteem</trd>,
    <trd>self-esteem</trd>,
    <trd>self respect</trd>,
    <trd>self-respect</trd>,
    <trd>self confidence</trd>,
    <trd>self-confidence</trd>
  </trdgrp>
  <trd lng="es">autoestima</trd>
  <trdgrp lng="fr">
    <trd>amour-propre</trd>,
    <trd>estime de soi</trd>
  </trdgrp>
  <trd lng="it">autostima</trd>
  <trd lng="pl">pewno&sacute;&cacute; siebie</trd>
  <trdgrp lng="sk">
    <trd>d&ocirc;stojnos&tcaron;</trd>,
    <trd>seba&uacute;cta</trd>
  </trdgrp>
</drv>
</art>
