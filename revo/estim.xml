<drv mrk="estim.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="estim.0i.KOMUNE">
    <dif>
      Alte <ref tip="vid" cel="taks.0i.KOMUNE">taksi<sncref/></ref>
      ies moralan a&ubreve; spiritan valoron:
      <ekz>
        la lernanto devas <tld/>i la instruanton
        <fnt>
          <bib>DL</bib>
          <lok>Dua Libro de l' Lingvo Internacia</lok>
        </fnt>;
      </ekz>
      <ekz>
        kia malalti&gcirc;o, <klr>[&#8230;]</klr>ke mi devas lin <tld/>i kiel sinjoron kaj li kun mi
        parolas tute senceremonie
        <fnt>
          <bib>DL</bib>
          <lok>Dua Libro de l' Lingvo Internacia</lok>
        </fnt>;
      </ekz>
      <ekz>
        se vi scius, kiu li estas, vi lin pli <tld/>us
        <fnt>
          <bib>F</bib>
          <lok>&FE; 26</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni &scirc;atas la talenton, oni <tld/>as la
        honestecon<fnt><bib>PV</bib></fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj liaj bonaj amikoj <klr>[&#8230;]</klr> forte lin <tld/>is
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> fari&gcirc;is bravaj homoj, kiujn &ccirc;iu <tld/>as
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>as la malri&ccirc;ulon, &scirc;i diris, &ccirc;e Dio li eble ricevos pli altan lokon, ol
        multaj el la ri&ccirc;uloj
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        bagatelo montris tion al mi: via princa mo&scirc;to <klr>[&#8230;]</klr> <tld/>as maljunecon
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvankam li ne estas jam sur la tero, mi <tld/>as lian nomon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>as viajn eksterordinarajn ecojn
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i sin mem <klr>[&#8230;]</klr>, tion mi komprenas
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XXV</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="admir.0i">admiri</ref>,
       <ref cel="aprez.0i">aprezi</ref>,
       <ref cel="prefer.0i">preferi</ref>,
       <ref cel="respek.0i">respekti</ref>,
       <ref cel="sxat.0i.aprezi">&scirc;ati<sncref/></ref>,
       <ref cel="simpat.0i">simpatii</ref>,
       <ref cel="taks.0i">taksi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_v;&c_a;&c_zh;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_u;&c_v;&c_a;&c_zh;&c_a;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_c;&c_je;&c_n;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_ch;&c_i;&c_t;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ct&iacute;t</trd>,
    <trd>m&iacute;t koho v &uacute;ct&ecaron;</trd>,
    <trd>v&aacute;&zcaron;it si</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>achten</trd>,
    <trd>sehr <ind>sch&auml;tzen</ind></trd>,
    <trd>hochsch&auml;tzen</trd>,
    <trd>hochachten</trd>,
    <trd>hoch <ind>achten</ind></trd>,
    <trd>ehren<klr> <klr tip="amb">(leterstilo)</klr></klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>esteem</trd>,
    <trd>regard</trd>,
    <trd>respect</trd>
  </trdgrp>
  <trd lng="es">estimar</trd>
  <trd lng="fr">estimer</trd>
