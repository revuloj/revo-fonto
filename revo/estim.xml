    <trd>stimare</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#23562;&#25964;&#12377;&#12427;  <pr>&#12381;&#12435;&#12369;&#12356;&#12377;&#12427;</pr></trd>,
    <trd>&#23562;&#37325;&#12377;&#12427;  <pr>&#12381;&#12435;&#12385;&#12423;&#12358;&#12377;&#12427;</pr></trd>,
    <trd>&#39640;&#12367;&#35413;&#20385;&#12377;&#12427;
    <pr>&#12383;&#12363;&#12367;&#12402;&#12423;&#12358;&#12363;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>achten</trd>,
    <trd>hoogachten</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>szanowa&cacute;</trd>,
    <trd>powa&zdot;a&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>estimar</trd>,
    <trd>avaliar</trd>,
    <trd>gostar de</trd>,
    <trd>prezar</trd>,
    <trd>apreciar</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_v;&c_a;&c_zh;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>cti&tcaron; si</trd>,
    <trd>preukazova&tcaron; &uacute;ctu</trd>,
    <trd>v&aacute;&zcaron;i&tcaron; si</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_v;&c_a;&c_zh;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_sh;&c_a;&c_n;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_c;&c_ib;&c_n;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="vo">stim&ouml;n</trd>
  <trdgrp lng="zh">
    <trd>&#29645;&#24859; <pr>zh&emacron;n&agrave;i</pr></trd>,
    <trd>&#37325;&#35222; <pr>zh&ograve;ngsh&igrave;</pr></trd>,
    <trd>&#39640;&#24230;&#35413;&#20729; <pr>g&amacron;od&ugrave;p&iacute;ngji&agrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="estim.0o">
  <kap><tld/>o</kap>
  <snc mrk="estim.0o.KOMUNE">
    <dif>
      Ago <tld/>i kaj rezulto de tiu ago:
      <ekz>
        estas pli preferinda <klr>[&#8230;]</klr> bona <tld/>o, ol ar&gcirc;ento kaj oro
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 22:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi per mia beko inspiras al ili <tld/>on kaj singardecon
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekzistas aliaj homoj <klr>[&#8230;]</klr> tute ne &gcirc;uantaj publikan <tld/>on
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        nobla amo estas bazita sur la <tld/>o, sed la <tld/>o estas haltigilo de la amo
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi kutimis rilati kun <tld/>o nur al tiaj argumentoj, kiuj estas <klr>[&#8230;]</klr>
        traplektitaj per multo da la&ubreve;taj a&ubreve;toritataj nomoj kaj brilas per
        <klr>[&#8230;]</klr> kvaza&ubreve;-sciencaj frazoj
        <fnt>
          <bib>EE</bib>
          <lok>I</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="respek.0o">respekto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_v;&c_a;&c_g;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_sh;&c_a;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_u;&c_v;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_ch;&c_i;&c_t;</trd>
  </trdgrp>
  <trd lng="cs">&uacute;cta</trd>
  <trdgrp lng="de">
    <trd>Wertsch&auml;tzung</trd>,
    <trd>Achtung</trd>,
    <trd>Hochachtung</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>esteem</trd>,
    <trd>regard</trd>,
    <trd>respect</trd>
  </trdgrp>
  <trd lng="es">estima</trd>
  <trd lng="fr">estime</trd>
  <trd lng="he">&he;&ayin;&resh;&kaf;&he;</trd>
  <trdgrp lng="hu">
    <trd>tisztelet</trd>,
    <trd>megbecs&uuml;l&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>harga</baz>penghargaan</trd>,
    <trd>hormat<klr> (n.)</klr></trd>,
    <trd><baz>hormat</baz>kehormatan</trd>,
    <trd><baz>mulia</baz>kemuliaan</trd>,
    <trd>respek</trd>,
    <trd>takzim<klr> (n.)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>rispetto</trd>,
    <trd>stima</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#23562;&#25964;  <pr>&#12381;&#12435;&#12369;&#12356;</pr></trd>,
    <trd>&#25964;&#24847;  <pr>&#12369;&#12356;&#12356;</pr></trd>,
    <trd>&#23562;&#37325;  <pr>&#12381;&#12435;&#12385;&#12423;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">achting</trd>
  <trdgrp lng="pl">
    <trd>szacunek</trd>,
    <trd>poszanowanie</trd>,
    <trd>powa&zdot;anie</trd>
  </trdgrp>
  <trd lng="pt">estima</trd>
  <trd lng="ru">&c_u;&c_v;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">&uacute;cta</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_v;&c_a;&c_g;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_sh;&c_a;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="vo">stim</trd>
  <trdgrp lng="zh">
    <trd>&#25964;&#24847; <pr>j&igrave;ngy&igrave;</pr></trd>,
    <trd>&#27427;&#36062;&#21147; <pr>x&imacron;nsh&#462;ngl&igrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="estim.0inda">
  <kap><tld/>inda</kap>
  <snc mrk="estim.0inda.KOMUNE">
    <dif>
      Meritanta <tld/>on:
      <ekz>
        <tld/>inda pensmaniero
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova strata lanterno</lok>
        </fnt>;
      </ekz>
      <ekz>
        honesta, <tld/>inda sincereco
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la metiejo <klr>[&#8230;]</klr> apartenis al maljuna <tld/>inda geedza paro
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis unu el la plej piaj kaj plej <tld/>indaj religiaj sa&gcirc;uloj
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Pri la homaranismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        multe da <tld/>indaj lokaj virinoj <klr>[&#8230;]</klr> penege ser&ccirc;as laboron kaj tiel
        malfacile &gcirc;in trovas
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi estas tro delikata, sed tio estas mia fiero, kaj &gcirc;i estas <tld/>inda
        <fnt>
          <bib>Fab2</bib>
          <lok>Flikilo</lok>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_v;&c_a;&c_zh;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_sh;&c_a;&c_n;&c_o;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_d;&c_o;&c_s;&c_t;&c_o;&c_je;&c_n; &c_z;&c_a;
  &c_u;&c_v;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="cs">&uacute;ctyhodn&yacute;</trd>
  <trdgrp lng="de">
    <trd>achtenswert</trd>,
    <trd>achtbar</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>estimable</trd>,
    <trd>respectable</trd>,
    <trd>worthy</trd>
  </trdgrp>
  <trd lng="es">estimable</trd>
  <trd lng="fr">estimable</trd>
  <trd lng="he">&resh;&alef;&vav;&yod; &lamed;&he;&ayin;&resh;&kaf;&he;</trd>
  <trd lng="hu">tisztelend&odblac;</trd>
  <trdgrp lng="id">
    <trd><ind>patut</ind> dihargai</trd>,
    <trd><ind>patut</ind> dihormati</trd>,
    <trd><ind>patut</ind> dimuliakan</trd>,
    <trd><ind>patut</ind> ditakzimkan</trd>
  </trdgrp>
  <trd lng="ja">&#23562;&#25964;&#12377;&#12409;&#12365;  <pr>&#12381;&#12435;&#12369;&#12356;&#12377;&#12409;&#12365;</pr></trd>
  <trd lng="nl">achtbaar</trd>
  <trdgrp lng="pl">
    <trd>szacowny</trd>,
    <trd>czcigodny</trd>
  </trdgrp>
  <trd lng="pt">estim&aacute;vel</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_s;&c_t;&c_o;&c_j;&c_n;&c_y;&c_j;
    &c_u;&c_v;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_ch;&c_t;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ctihodn&yacute;</trd>,
    <trd>&uacute;ctyhodn&yacute;</trd>
  </trdgrp>
  <trd lng="uk">&c_g;&c_ib;&c_d;&c_n;&c_i;&c_j; &c_p;&c_o;&c_v;&c_a;&c_g;&c_i;</trd>
  <trd lng="zh">&#20516;&#24471;&#23562;&#37325; <pr>zh&iacute;dez&umacron;nzh&ograve;ng</pr></trd>
</drv>

<drv mrk="estim.mal0i">
  <kap><ofc>*</ofc>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="estim.mal0i.KOMUNE">
    <dif>
      Malalte taksi ies moralan a&ubreve; spiritan valoron:
      <ekz>
        ni forte mal<tld/>as tiun &ccirc;i malnoblan homon
        <fnt>
          <bib>F</bib>
          <lok>&FE; 33</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj, kiuj &scirc;in <tld/>is, nun mal<tld/>as &scirc;in, &ccirc;ar ili vidas &scirc;ian malhonoron
        <fnt>
          <bib>MT</bib>
          <lok>Plorkanto 1:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        sa&gcirc;on kaj instruon malsa&gcirc;uloj mal<tld/>as
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 1:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        Vi faris nin mal<tld/>ata&jcirc;o por niaj najbaroj
        <fnt><bib>MT</bib> <lok>&Psa; 44:13</lok></fnt>;
      </ekz>
      <ekz>
        mi faris nenion, kion mal<tld/>us la dioj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi ne devas mal<tld/>i tiun, kiu helpis vin en malfeli&ccirc;o
        <fnt>
          <bib>ElektFab</bib>
          <lok>Re&gcirc;o rano a&ubreve; fera Henriko</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi tiel mal<tld/>as la pre&gcirc;on, sed memoru, memoru! kiam la mizero al iu venas,
        kiam la akvo venos al la gor&gcirc;o, vi fordonos &ccirc;iujn trezorojn de la mondo por
        unu kristana &gcirc;emeto
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="abomen.0i">abomeni</ref>,
      <ref cel="sxat.mal0i">mal&scirc;ati</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_g;&c_a;&c_r;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_p;&c_r;&c_je;&c_z;&c_i;&c_r;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>nen&aacute;vid&ecaron;t</trd>,
    <trd>opovrhovat</trd>,
    <trd>pohrdat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gering <ind>sch&auml;tzen</ind></trd>,
    <trd>verachten</trd>,
    <trd>missachten</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>disdain</trd>,
    <trd>hold in contempt</trd>,
    <trd>disrespect</trd>,
    <trd>despise</trd>
  </trdgrp>
  <trd lng="es">desestimar</trd>
  <trd lng="fr">m&eacute;priser</trd>
  <trd lng="he">&lamed;&bet;&vav;&zayin;</trd>
  <trdgrp lng="hu">
    <trd>megvet</trd>,
    <trd>len&eacute;z</trd>,
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>cela</baz>mencela</trd>,
    <trd><baz>cemooh</baz>mencemooh</trd>,
    <trd><baz>ejek</baz>mengejek</trd>,
    <trd><baz>hina</baz>menghina</trd>,
    <trd><baz>hina</baz>menghinakan</trd>,
    <trd><baz>keji</baz>mengejikan</trd>,
    <trd><baz>nista</baz>menistakan</trd>,
    <trd><baz>pandang</baz>memandang hina</trd>,
    <trd><baz>pandang</baz>memandang rendah</trd>,
    <trd><baz>remeh</baz>meremehkan</trd>,
    <trd><baz>rendah</baz>merendahkan</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>disprezzare</trd>,
    <trd>disdegnare</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#36605;&#34065;&#12377;&#12427;  <pr>&#12369;&#12356;&#12409;&#12388;&#12377;&#12427;</pr></trd>,
