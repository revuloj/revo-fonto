<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kampan1.xml,v 1.27 2023/12/02 21:20:05 revo Exp $">
<kap>
  <rad>kampanil</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kampan1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ARKI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="tur.0o.ARKI">Turo</ref>
      memstara a&ubreve; surstaranta konstruon kaj en kiu oni lokas
      <ref tip="prt" cel="sonor.0ilo">sonorilojn</ref>
      a&ubreve; <ref tip="prt" cel="horlog.0o">horlo&gcirc;on</ref>:  
      <ekz>
        en la historia centro de <nom>&Hcirc;arkov</nom> <klr>[&#x2026;]</klr> estis
        konstruita la kirko 
        de la Dipatrina &Ccirc;ieliro <klr>[&#x2026;]</klr> post la rusa-franca
        milito de 1812,
        dedi&ccirc;e al la venko de rusaj milittrupoj, oni lan&ccirc;is konstruadon
        de <tld/>o
        (la popolo poste nomis &gcirc;in &leftquot;La <tld/>o de 1812&rightquot;), <klr>[&#x2026;]</klr>
        sur la
        <tld/>a supro estis instalita melodie sonoranta horlo&gcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Dmitrij Cibulevskij</aut>
          <vrk><url ref="&Monato;/2004/008012.html">Katedralo renaski&gcirc;anta</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_v;&c_a;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="bg">&c_k;&c_a;&c_m;&c_b;&c_a;&c_n;&c_a;&c_r;&c_i;&c_ja;</trd>
  <trd lng="ca">campanar</trd>
  <trdgrp lng="cs">
    <trd>kampanila</trd>,
    <trd>zvonice</trd>
  </trdgrp>
  <trd lng="de">Glockenturm</trd>
  <trdgrp lng="en">
    <trd>campanile</trd>,
    <trd>bell tower</trd>,
    <trd>clock tower</trd>
  </trdgrp>
  <trd lng="es">campanario</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_r;&a_j; &a_n;&a_A;&a_q;&a_w;&a_s;</trd>
    <trd>&a_b;&a_r;&a_j; &a_s;&a_A;&a_ayn;&a_t;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>campanile</trd>,
    <trd>clocher<klr tip="ind"> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>harangtorony</trd>,
    <trd>&oacute;ratorony</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x9418;&#x5854;  <pr>&#x3057;&#x3087;&#x3046;&#x3068;&#x3046;</pr></trd>,
    <trd>&#x6642;&#x8A08;&#x5854;  <pr>&#x3068;&#x3051;&#x3044;&#x3068;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">klokkentoren</trd>
  <trd lng="pl">dzwonnica</trd>
  <trd lng="ro">clopotni&#x21B;&abreve;</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_o;&c_k;&c_o;&c_l;&c_mol;&c_n;&c_ja;</trd>
  <trd lng="sk">zvonica</trd>
  <trdgrp lng="uk">
    <trd>&#x434;&#x437;&#x432;&#x456;&#x43D;&#x438;&#x446;&#x44F;</trd>,
    <trd>&#x43A;&#x430;&#x43C;&#x43F;&#x430;&#x43D;&#x456;&#x43B;&#x430;</trd>,
    <trd>&#x432;&#x435;&#x436;&#x430;</trd>
  </trdgrp>
  <trd lng="zh">&#x9418;&#x6A13; <pr>zh&#x14D;ngl&#xF3;u</pr></trd>
</drv>
</art>
