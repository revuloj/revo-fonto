    <trd>&#x5B89;&#x3089;&#x304B;&#x3055;  <pr>&#x3084;&#x3059;&#x3089;&#x304B;&#x3055;</pr></trd>
  </trdgrp>
  <trd lng="pl">spok&oacute;j</trd>
  <trd lng="ru">&c_s;&c_p;&c_o;&c_k;&c_o;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>k&lcaron;ud</trd>,
    <trd>pokoj</trd>
