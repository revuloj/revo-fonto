    <trd>&#x5B89;&#x7136; <pr>&#x101;nr&#xE1;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="trankv.0o">
  <kap><tld/>o</kap>
  <snc mrk="trankv.0o.anima">
    <dif>
      <tld lit="T"/>a animstato:
      <ekz>
        lia koro neniam &gcirc;uos <tld/>on
          <fnt>
            <bib>Far3</bib>
            <lok>&Ccirc;apitro XI</lok>
          </fnt>;
      </ekz>
      <ekz>
        se per ia bona faro al vi <tld/>on povas mi alporti,
        parolu
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        la infanoj donas al la homoj momenton de forgeso kaj
        <tld/>o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        trovi <tld/>on en la penso, ke...
      </ekz>
    </dif>
    <trdgrp lng="nl">
      <trd>bedaardheid</trd>,
      <trd>gerustheid</trd>,
      <trd>kalmte</trd>
    </trdgrp>
  </snc>
  <snc mrk="trankv.0o.kvieto">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="kviet.0o.paco">Kvieto<sncref/></ref>:
      <ekz>
       sur la kamparo regis <tld/>o.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="trankv.0eco"><tld/>eco</ref>
  </refgrp>

  <trd lng="be">&c_s;&c_p;&c_a;&c_k;&c_o;&c_j;</trd>
  <trd lng="cs">klid</trd>
  <trdgrp lng="en">
    <trd>tranquility</trd>,
    <trd>calm<klr> (noun)</klr></trd>
  </trdgrp>
  <trd lng="es">tranquilidad</trd>
  <trd lng="fr">tranquillit&eacute;</trd>
  <trd lng="hu">nyugalom</trd>
  <trd lng="id"><baz>tenang</baz>ketenangan</trd>
  <trdgrp lng="ja">
    <trd>&#x5E73;&#x9759;  <pr>&#x3078;&#x3044;&#x305B;&#x3044;</pr></trd>,
    <trd>&#x6C88;&#x7740;  <pr>&#x3061;&#x3093;&#x3061;&#x3083;&#x304F;</pr></trd>,
    <trd>&#x843D;&#x3061;&#x7740;&#x304D;  <pr>&#x304A;&#x3061;&#x3064;&#x304D;</pr></trd>,
    <trd>&#x5B89;&#x5FC3;  <pr>&#x3042;&#x3093;&#x3058;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="pl">bezpiecze&nacute;stwo</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_o;&c_k;&c_o;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_o;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>k&lcaron;ud</trd>,
    <trd>pokoj</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_p;&c_o;&c_k;&c_ib;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x5B89;&#x5B81; <pr>&#x101;nn&#xED;ng</pr></trd>,
    <trd>&#x5B89;&#x5BE7; <pr>&#x101;nn&#xED;ng</pr></trd>,
    <trd>&#x5B81;&#x9759; <pr>n&#xED;ngj&#xEC;ng</pr></trd>,
