<drv mrk="trankv.0igi">
  <kap><ofc>*</ofc><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Igi iun a&ubreve; ion <ref cel="trankv.0a.anime">trankvila<sncref/></ref>:
      <ekz>
        <tld/>igi la malpaciencajn
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igu por momento vian miron
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igi la &ccirc;agrenon de iu;
      </ekz>
      <ekz>
        mi rapidos nun <tld/>igi la amikojn, kiuj atendas
        sopirante
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        donu al li <tld/>igajn konsilojn;
      </ekz>
      <ekz>
        la bona virino <tld/>igis sian soifon
        <fnt><bib>F</bib>, <lok>&FE; 15</lok></fnt>;
      </ekz>
      <ekz>
        mi <tld/>igis kaj kvietigis mian animon
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 131:2</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_p;&c_a;&c_k;&c_o;&c_j;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>konej&scaron;it</trd>,
    <trd>uklid&ncaron;ovat</trd>
  </trdgrp>
  <trd lng="de">beruhigen</trd>
  <trdgrp lng="en">
    <trd>calm<klr> (tr)</klr></trd>,
    <trd>calm down<klr> (tr)</klr></trd>
  </trdgrp>
  <trd lng="es">tranquilizar</trd>
  <trd lng="fr">tranquilliser</trd>
  <trdgrp lng="hu">
    <trd>nyugtat</trd>,
    <trd>megnyugtat</trd>
  </trdgrp>
  <trd lng="id"><baz>tenang</baz>menenangkan</trd>
  <trdgrp lng="ja">
    <trd>&#x9759;&#x3081;&#x308B;  <pr>&#x3057;&#x305A;&#x3081;&#x308B;</pr></trd>,
    <trd>&#x843D;&#x3061;&#x7740;&#x304B;&#x305B;&#x308B;  <pr>&#x304A;&#x3061;&#x3064;&#x304B;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x5B89;&#x5FC3;&#x3055;&#x305B;&#x308B;  <pr>&#x3042;&#x3093;&#x3057;&#x3093;&#x3055;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>geruststellen</trd>,
    <trd>kalmeren</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ukoi&cacute;</trd>,
    <trd>uspokaja&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_p;&c_o;&c_k;&c_a;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_s;&c_p;&c_o;&c_k;&c_o;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>uk&lcaron;udni&tcaron;</trd>,
    <trd>upokoji&tcaron;</trd>,
    <trd>ut&iacute;&scaron;i&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_s;&c_p;&c_o;&c_k;&c_o;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_u;&c_t;&c_i;&c_h;&c_o;&c_m;&c_i;&c_r;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x93AE;&#x975C; <pr>zh&#xE8;nj&#xEC;ng</pr></trd>,
    <trd>&#x9547;&#x9759; <pr>zh&#xE8;nj&#xEC;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="trankv.0ige">
  <kap>
    <tld/>ige
  </kap>
  <snc>
    <dif>
      En <tld/>iga maniero:
      <ekz>
        &leftquot;ne gravas,&rightquot; O&jcirc;o diris <tld/>ige
        <fnt>
          <aut>L. Frank Baum (tr. Donald Broadribb)</aut>
          <vrk><url ref="https://www.rulit.me/books/la-miks-read-255626-18.html">La Miks&ccirc;ifona
          Knabino de Oz</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la vegeta&jcirc;oj efikas kune kun irado aparte <tld/>ige
        <fnt>
          <vrk><url ref="https://esperantaretradio.blogspot.com/2020/05/promeni-felicigas.html">Esperanta
          Retradio (20 Majo 2020)</url></vrk>
          <lok>Promeni feli&ccirc;igas</lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;tio estas nur optika fenomeno&rightquot;, li diris <tld/>ige al si mem
        <fnt>
          <aut>Hans-Georg Kaiser</aut>
          <vrk><url ref="https://satesperanto.org/spip.php?article2997">SAT Kulturo</url></vrk>
          <lok>Malicaj kristnaskaj an&gcirc;eloj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_p;&c_a;&c_k;&c_o;&c_j;&c_l;&c_ib;&c_v;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_p;&c_a;&c_k;&c_o;&c_j;&c_v;&c_a;&c_ju;&c_ch;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>beruhigend</trd>,
