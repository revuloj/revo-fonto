<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: jakob.xml,v 1.19 2023/12/03 07:09:06 revo Exp $">
<kap>
  <rad>Jakob</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="jakob.0o">
  <kap><tld/>o,
  <var><kap><tld/></kap></var></kap>
  <snc mrk="jakob.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> hebredevena:
      <ekz>
        <tld/>o respondis, dirante: Fratoj, a&ubreve;skultu min
        <fnt><bib>NT</bib>, <lok>&Ago; 15:13</lok></fnt>.
      </ekz>
    </dif>          
    <trdgrp lng="br">
      <trd>Jalm</trd>,
      <trd>Jakez</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_Ja;&c_k;&c_o;&c_v;</trd>,
      <trd>&c_Ja;&c_k;&c_o;&c_b;<klr> (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)
      </klr></trd>,
      <trd>&c_Zh;&c_a;&c_k;<klr> (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr>
      </trd>
    </trdgrp>
  </snc>
  <snc mrk="jakob.0o.BIB">
    <uzo tip="fak">BIB</uzo>
    <uzo tip="fak">PERS</uzo>
    <dif>
      Biblia patriarko, dua filo de Isaako, patro de dekdu filoj
      rigardataj kiel prauloj de la dekdu izraelaj triboj:
      <ekz>
        <tld/> fari&gcirc;is homo kvieta, sidanta en la tendo
        <fnt><bib>MT</bib>, <lok>&Gen; 25:27</lok></fnt>.
      </ekz>
    </dif>                    
    <trd lng="br">Jakob</trd>
    <trd lng="ru">&c_I;&c_a;&c_k;&c_o;&c_v;</trd>
  </snc>
  <trd lng="be">&c_Ja;&c_k;&c_u;&c_b;</trd>
  <trdgrp lng="ca">
    <trd>Jacob</trd>,
    <trd>Jaume</trd>
  </trdgrp>
  <trd lng="de">Jakob</trd>
  <trd lng="fa">&f_y;&a_ayn;&a_q;&a_w;&a_b;</trd>
  <trdgrp lng="fr">
    <trd>Jacob</trd>,
    <trd>Jacques</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>J&aacute;kob</trd>,
    <trd>Jakab</trd>
  </trdgrp>
  <trd lng="id">Yakub</trd>
  <trdgrp lng="ja">
    <trd>&#x30E4;&#x30B3;&#x30D6;</trd>,
    <trd>&#x30B8;&#x30A7;&#x30A4;&#x30B3;&#x30D6;</trd>
  </trdgrp>
  <trd lng="nl">Jacob</trd>
  <trd lng="pl">Jakub</trd>
  <trdgrp lng="pt">
    <trd>Jac&oacute;</trd>,
    <trd>Jacob</trd>,
  </trdgrp>
  <trd lng="sv">Jakob</trd>
  <trd lng="it">Giacobbe</trd>
  <trdgrp lng="uk">
    <trd>&#x42F;&#x43A;&#x456;&#x432;</trd>,
    <trd>&#x414;&#x436;&#x435;&#x43A;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x52A0;&#x79D1;&#x5E03; <pr>ji&#x101;k&#x113;b&#xF9;</pr></trd>,
    <trd>&#x8A79;&#x59C6;&#x58EB; <pr>zh&#x101;nm&#x1D4;sh&#xEC;</pr></trd>,
    <trd>&#x96C5;&#x5404; <pr>y&#x1CE;g&#xE8;</pr></trd>,
    <trd>&#x5091;&#x5BA2; <pr>ji&#xE9;k&#xE8;</pr></trd>
  </trdgrp>
</drv>
</art>
