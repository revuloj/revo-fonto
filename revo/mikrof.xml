<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mikrofarad</rad>/o
</kap>
<drv mrk="mikrof.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de kapacitanco, 
      milionono de <ref tip="malprt" cel="farad.f0o">farado</ref>;
      simb. &my;F:
      <ekz>
        la <tld/>o estas tre ofte uzata por mezuri la kapacitojn en 
        radioelektro.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Mikrofarad</trd>
  <trd lng="en">microfarad</trd>
  <trd lng="fr">microfarad</trd>
  <trd lng="hu">mikrofarad</trd>
  <trd lng="nl">microfarad</trd>
  <trd lng="ru">&c_m;&c_i;&c_k;&c_r;&c_o;&c_f;&c_a;&c_r;&c_a;&c_d;&c_a;</trd>
</drv>
</art>
