<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>metamorfoz</rad>/o
</kap>
<drv mrk="metamo1.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Transformi&gcirc;o de iu esta&jcirc;o en alian:
      <ekz>
        la <tld/>oj de Jupitero.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Aliformi&gcirc;o:
      <ekz>
        <tld/>o de ra&ubreve;po en pupon kaj en papilion.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plena a&ubreve; grava &scirc;an&gcirc;o en la stato a&ubreve; ecoj de iu:
      <ekz>
        okazis feli&ccirc;a <tld/>o de lia karaktero.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="nl">
    <trd>gedaanteverandering</trd>,
    <trd>metamorfose</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_t;&c_a;&c_m;&c_o;&c_r;&c_f;&c_o;&c_z;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_v;&c_r;&c_a;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="metamo1.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Transformi.
    </dif>
  </snc>
  <trd lng="nl">van <ind>gedaante</ind> doen veranderen</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_v;&c_r;&c_a;&c_shch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_v;&c_r;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
