<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kinkonin</rad>/o
</kap>
<drv mrk="kinkon1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Unu el la alkaloidaj substancoj
      en <ref cel="kinkon.0o.KEM">kinkono</ref>:
      <ekz>
        la <tld/>o estas uzata en la medicino plej ofte
        kiel <tld/>a sulfato.
      </ekz>
    </dif>
    <ref tip="vid" cel="hxinin.0o">&hcirc;inino</ref>
  </snc>
  <trd lng="de">Zinchonin</trd>
  <trd lng="fr">cinchonine</trd>
  <trd lng="hu">cinkonin</trd>
  <trd lng="nl">cinchonine</trd>
</drv>
</art>
