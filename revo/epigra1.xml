<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: epigra1.xml,v 1.24 2023/11/07 20:34:11 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>epigram</rad>/o
</kap>
<drv mrk="epigra1.0o">
  <kap><tld/>o</kap>
  <snc mrk="epigra1.0o.POE">
    <uzo tip="fak">BELE</uzo>
    <uzo tip="fak">POE</uzo>
    <dif>
      Tre mallonga <ref tip="super" cel="poem.0o.POE">poemo</ref>
      esprimanta satiran kritikon:
      <ekz>
        <nom>T&aacute;rkony</nom> regis la soneton petrarkan kaj la iom <tld/>an
        mallongan poemon
        <fnt><bib>Plumamikoj</bib>, <lok>esperanta poezio: mirakla
        rikolto</lok></fnt>;
      </ekz>
      <ekz>
        <nom>John Donne</nom> en 1624 a&ubreve;toris medita&jcirc;on, kiu poste fami&gcirc;is tutmonde kiel <tld/>o
        por romano de <nom>Ernest Hemingway</nom>: &leftquot;neniu homo estas insulo&rightquot;, &gcirc;i komenci&gcirc;as
        <fnt>
          <vrk><url ref="https://www.glasnost.se/temo/recenzoj/">recenzoj - Glasnost.se</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="epigra.0o.moto">epigrafo<sncref/></ref>
  </snc>
  <trd lng="be">&c_e;&c_p;&c_ib;&c_g;&c_r;&c_a;&c_m;&c_a;</trd>
  <trd lng="ca">epigrama</trd>
  <trdgrp lng="cs">
    <trd>epigram</trd>,
    <trd>jadrn&aacute; b&aacute;sni&ccaron;ka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Sinngedicht</trd>,
    <trd>Spottgedicht</trd>,
    <trd>Epigramm</trd>
  </trdgrp>
  <trd lng="en">epigram</trd>
  <trd lng="es">epigrama</trd>
  <trdgrp lng="fa">
    <trd>&a_h;&a_j;&a_w;&a_n;&a_A;&a_m;&a_h;</trd>
    <trd>&a_h;&a_j;&a_w;&f_y;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">&eacute;pigramme</trd>
  <trd lng="hu">epigramma</trd>
  <trdgrp lng="ja">
    <trd>&#x8B66;&#x53E5;  <pr>&#x3051;&#x3044;&#x304F;</pr></trd>,
    <trd>&#x30A8;&#x30D4;&#x30B0;&#x30E9;&#x30E0;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>fraszka</trd>,
    <trd>epigram</trd>
  </trdgrp>
  <trd lng="pt">epigrama</trd>
  <trd lng="ru">&c_e;&c_p;&c_i;&c_g;&c_r;&c_a;&c_m;&c_m;&c_a;</trd>
  <trd lng="sk">epigram</trd>
  <trd lng="uk">&#x435;&#x43F;&#x456;&#x433;&#x440;&#x430;&#x43C;&#x430;</trd>
  <trdgrp lng="zh">
    <trd>&#x8AF7;&#x523A;&#x77ED;&#x8A69; <pr>f&#xE8;ngc&#xEC;du&#x1CE;nsh&#x12B;</pr></trd>,
    <trd>&#x8AFA;&#x8A9E; <pr>y&#xE0;ny&#x1D4;</pr></trd>
  </trdgrp>
</drv>
</art>
