<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>epigram</rad>/o
</kap>
<drv mrk="epigra1.0o">
  <kap><tld/>o</kap>
  <snc mrk="epigra1.0o.POE">
    <uzo tip="fak">BELE</uzo>
    <uzo tip="fak">POE</uzo>
    <dif>
      Tre mallonga <ref tip="super" cel="poem.0o.POE">poemo</ref>
      esprimanta satiran kritikon:
      <ekz>
        T&aacute;rkony regis la soneton petrarkan kaj la iom <tld/>an mallongan
        poemon
        <fnt><bib>Plumamikoj</bib>, <lok>esperanta poezio: mirakla
        rikolto</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_p;&c_ib;&c_g;&c_r;&c_a;&c_m;&c_a;</trd>
  <trd lng="ca">epigrama</trd>
  <trdgrp lng="de">
    <trd>Sinngedicht</trd>,
    <trd>Spottgedicht</trd>,
    <trd>Epigramm</trd>
  </trdgrp>
  <trd lng="es">epigrama</trd>
  <trd lng="fr">&eacute;pigramme</trd>
  <trd lng="hu">epigramma</trd>
  <trd lng="ru">&c_e;&c_p;&c_i;&c_g;&c_r;&c_a;&c_m;&c_m;&c_a;</trd>
</drv>
</art>
