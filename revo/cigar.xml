<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cigar.xml,v 1.32 2023/10/31 13:37:03 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>cigar</rad>/o
</kap>

<drv mrk="cigar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Longforma kunvolva&jcirc;o el sekigitaj
      <ref tip="prt" cel="tabak.0o">tabak</ref>folioj por fumi:
      <ekz>
        pla&ccirc;as al mi la blueta fumo de l' <tld/>o, kiun vi fumas
        <fnt>
          <bib>DL</bib>
          <lok>Dua Libro de l' Lingvo Internacia</lok>
        </fnt>;
      </ekz>
      <ekz>
        for&jcirc;etinte la pecon da <tld/>o, kiun li estis teninta inter la fingroj, li
        ekrigardis min
        <fnt>
          <bib>DL</bib>
          <lok>Dua Libro de l' Lingvo Internacia</lok>
        </fnt>;
      </ekz>
      <ekz>
        Petro <klr>[...]</klr> donis al li kun la skatoleto nigran
        <tld/>on
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 3a, p.
        16a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="cigare.0o">cigaredo</ref>
    <trd lng="be">&c_c;&c_y;&c_g;&c_a;&c_r;&c_a;</trd>
    <trd lng="br">segalenn<klr tip="ind"> (da vutunat)</klr></trd>
    <trd lng="ca">cigar</trd>
    <trd lng="de">Zigarre</trd>
    <trd lng="el">&pi;&omikron;&ypsilon_ton;&rho;&omikron;</trd>
    <trd lng="en">cigar</trd>
    <trd lng="es">cigarro puro</trd>
    <trd lng="fr">cigare</trd>
    <trd lng="hu">szivar</trd>
    <trd lng="it">sigaro</trd>
    <trd lng="nl">sigaar</trd>
    <trd lng="pl">cygaro</trd>
    <trd lng="pt">charuto</trd>
    <trd lng="ru">&c_s;&c_i;&c_g;&c_a;&c_r;&c_a;</trd>
    <trd lng="sk">cigara</trd>
  </snc>
  <trd lng="cs">doutn&iacute;k</trd>
  <trd lng="he">&samekh;&#x5B4;&yod;&gimel;&#x5B8;&resh;</trd>
  <trd lng="ja">&#x8449;&#x5DFB;  <pr>&#x306F;&#x307E;&#x304D;</pr></trd>
  <trd lng="uk">&c_s;&c_i;&c_gu;&c_a;&c_r;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x96EA;&#x8304; <pr>xu&#x11B;ji&#x101;</pr></trd>,
    <trd>&#x96EA;&#x8304;&#x70DF; <pr>xu&#x11B;ji&#x101;y&#x101;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="cigar.0ejo">
<kap><ofc>*</ofc><tld/>ejo</kap>
  <snc>
    <dif>
      Butiko, en kiu oni vendas <tld/>ojn:
      <ekz>
      Magazeno, en kiu oni vendas cigarojn, a&ubreve; &ccirc;ambro, en kiu oni tenas
      cigarojn, estas cigarejo <fnt><bib>F</bib>, <lok>&FE;
      40</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_g;&c_a;&c_r;&c_n;&c_a;&c_ja; &c_k;&c_r;&c_a;&c_m;&c_a;</trd>
  <trdgrp lng="ja">
    <trd>&#x8449;&#x5DFB;&#x5DE5;&#x5834;  <pr>&#x306F;&#x307E;&#x304D;&#x3053;&#x3046;&#x3058;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x8449;&#x5DFB;&#x58F2;&#x5E97;  <pr>&#x306F;&#x307E;&#x304D;&#x3070;&#x3044;&#x3066;&#x3093;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="cigar.0ingo">
<kap><ofc>*</ofc><tld/>ingo</kap>
  <snc>
    <dif>
      Tubeto, en kiun oni metas <tld/>on por fumi:
      <ekz>
        tubeto, en kiun oni metas <tld/>on, kiam oni &gcirc;in fumas,
        estas <tld/>ingo
        <fnt><bib>F</bib>, <lok>&FE; 40</lok></fnt>;
      </ekz>
      <ekz>
        ni estis en speco de butiko, kie <klr>[&#x2026;]</klr> tabakujoj, <tld/>ingoj, tabako,
        <tld/>oj kaj cetere estis a&ccirc;eteblaj
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <adm>
      Kontroli la tradukojn nl: mi kunfandis tiun de
      sub "ing" kaj tiun de sub "cigar".
    <aut>MB</aut>
    </adm>
    <trd lng="be">&c_m;&c_u;&c_sh;&c_t;&c_u;&c_k;</trd>
    <trd lng="br">doug-segalenn</trd>
    <trd lng="ca">broquet</trd>
    <trd lng="de">Zigarrenhalter</trd>
    <trd lng="en">cigar holder</trd>
    <trd lng="es">boquilla</trd>
    <trd lng="fr">fume-cigare</trd>
    <trd lng="hu">szipka</trd>
    <trd lng="it">bocchino</trd>
    <trdgrp lng="nl">
    <trd>sigarenhouder</trd>,
    <trd>sigarenpijpje</trd>
    </trdgrp>
    <trd lng="pl">cygarniczka</trd>
    <trd lng="pt">piteira</trd>
    <trd lng="ru">&c_m;&c_u;&c_n;&c_d;&c_sh;&c_t;&c_u;&c_k;</trd>
  </snc>
  <trd lng="cs">&scaron;pi&ccaron;ka na doutn&iacute;ky</trd>
  <trd lng="he">&pe;&vav;&mem;&yod;&tav; &lamed;&samekh;&yod;&gimel;&resh;</trd>
  <trd lng="ja">&#x8449;&#x5DFB;&#x30D1;&#x30A4;&#x30D7;  <pr>&#x306F;&#x307E;&#x304D;&#x3071;&#x3044;&#x3077;</pr></trd>
  <trd lng="sk">&scaron;pi&ccaron;ka na cigary</trd>
  <trd lng="uk">&c_m;&c_u;&c_n;&c_d;&c_sh;&c_t;&c_u;&c_k;</trd>
  <trd lng="zh">&#x70DF;&#x5634;&#x513F; <pr>y&#x101;nzu&#x1D0;&#xE9;r</pr></trd>
</drv>

<drv mrk="cigar.0ujo">
  <kap><ofc>*</ofc><tld/>ujo</kap>
  <snc>
    <dif>
      Skatoleto, en kiu oni konservas a&ubreve; portas <tld/>ojn:
      <ekz>
        skatoleto a&ubreve; alia objekto, en kiu oni tenas cigarojn,
        estas <tld/>ujo
        <fnt><bib>F</bib>, <lok>&FE; 40</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_r;&c_t;&c_s;&c_ib;&c_g;&c_a;&c_r;</trd>
    <trd lng="br">boest <ind>segalenn</ind></trd>
    <trdgrp lng="ca">
      <trd>cigarrera</trd>,
      <trd>portacigars</trd>
    </trdgrp>
    <trd lng="de">Zigarrenkiste</trd>
    <trd lng="el">&tau;&sigma;&jota;&gamma;&alfa;&rho;&omikron;&theta;&eta_ton;&kappa;&eta;</trd>
    <trd lng="en">cigar box</trd>
    <trd lng="es">purera</trd>
    <trd lng="fr">porte-cigare</trd>
    <trd lng="hu">szivardoboz</trd>
    <trd lng="it">portasigari</trd>
    <trd lng="nl">sigarenkist</trd>
    <trd lng="pl">pude&lstroke;ko <ind>cygar</ind></trd>
    <trd lng="pt">cigarreira</trd>
    <trd lng="ru">&c_p;&c_o;&c_r;&c_t;&c_s;&c_i;&c_g;&c_a;&c_r;</trd>
  </snc>
  <trd lng="cs">pouzdro na doutn&iacute;ky</trd>
  <trd lng="he">&qof;&vav;&pe;&samekh;&tav;
  &samekh;&yod;&gimel;&resh;&yod;&vav;&tav;</trd>
  <trd lng="ja">&#x8449;&#x5DFB;&#x5165;&#x308C;  <pr>&#x306F;&#x307E;&#x304D;&#x306F;&#x3044;&#x308C;</pr></trd>
  <trd lng="sk">puzdro na cigary</trd>
  <trd lng="uk">&c_s;&c_i;&c_gu;&c_a;&c_r;&c_n;&c_i;&c_c;&c_ja;</trd>
  <trd lng="zh">&#x96EA;&#x8304;&#x76D2; <pr>xu&#x11B;ji&#x101;h&#xE9;</pr></trd>
</drv>

</art>

