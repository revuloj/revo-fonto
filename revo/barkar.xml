<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: barkar.xml,v 1.21 2022/04/26 15:00:32 revo Exp $">
<kap><rad>barkarol</rad>/o
<fnt>Z</fnt></kap>
<drv mrk="barkar.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="barkar.0o.kanto">
    <dif>
      Formo de tradicia kanto de Veneciaj gondolistoj,
      kutime la&ubreve; malsimpla takto, kiu imitas la movadon de boato iranta
      tra la akvo.
    </dif>
  </snc>
  <snc mrk="barkar.0o.komponajho">
     <dif>
       Muzika kompona&jcirc;o kun tia stilo:
       <ekz>
        <tld/>o: poemo de J. Baghy, muziko de M. S&aacute;rosi
             <fnt>
        <vrk>Literatura Mondo, numero 2, Novembro 1922</vrk>,
        <lok><url ref="&Gutenberg;ebooks/52063">
        gutenberg.org</url></lok>
        </fnt>.
      </ekz>
      </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_r;&c_k;&c_a;&c_r;&c_o;&c_l;&c_a;</trd>
  <trd lng="bg">&c_b;&c_a;&c_r;&c_k;&c_a;&c_r;&c_o;&c_l;&c_a;</trd>
  <trd lng="ca">barcarola</trd>
  <trdgrp lng="de">
    <trd>Barkarole</trd>,
    <trd>Gondellied</trd>
  </trdgrp>
  <trd lng="en">barcarolle</trd>
  <trd lng="es">barcarola</trd>
  <trd lng="fr">barcarolle</trd>
  <trdgrp lng="hu">
    <trd>barcarola</trd>,
    <trd>b&aacute;rka&eacute;nek</trd>
  </trdgrp>
  <trd lng="it">barcarola</trd>
  <trdgrp lng="nl">
    <trd>barcarole</trd>,
    <trd>gondellied</trd>
  </trdgrp>
  <trd lng="pl">barkarola</trd>
  <trd lng="ru">&c_b;&c_a;&c_r;&c_k;&c_a;&c_r;&c_o;&c_l;&c_a;</trd>
  <trd lng="cs">barkarola</trd>
  <trd lng="sk">barkarola</trd>
  <trd lng="uk">&#x431;&#x430;&#x440;&#x43A;&#x430;&#x440;&#x43E;&#x43B;&#x430;</trd>
</drv>
</art>
