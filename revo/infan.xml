<drv mrk="infan.0istino">
  <kap><ofc>*</ofc><tld/>istino</kap>
  <snc mrk="infan.0istino.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      <ref tip="dif" cel="vart.0istino">Vartistino</ref>:
      <ekz>
        ni havas diversajn servantojn: kuiriston, &ccirc;ambristinon, 
        <tld/>istinon
        <fnt><bib>F</bib>, <lok>&FE; 37</lok></fnt>;
      </ekz>
      <ekz>
        neniu atentis la arbon, esceptinte la maljunan <tld/>istinon, 
        kiu rigardis atente inter la bran&ccirc;ojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Abio</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_n;&c_mol;&c_k;&c_a;</trd>,
    <trd>&c_n;&c_ja;&c_n;&c_je;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_h;&c_a;&c_v;&c_a;&c_w;&c_n;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>mainadera</trd>,
    <trd>puericultora</trd>
  </trdgrp>
  <trd lng="cs">ch&uring;va</trd>
  <trdgrp lng="de">
