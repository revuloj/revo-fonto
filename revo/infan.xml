<drv mrk="infan.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="infan.0eto.bebo">
    <dif>
      Malgranda, juna infano, <ref tip="vid" cel="et.0ulo">etulo</ref> 
      a&ubreve;
      <ref tip="vid" cel="beb.0o">bebo</ref>:
      <ekz>
        oni venigis al li anka&ubreve; siajn <tld/>etojn, por ke li 
        tu&scirc;u ilin
        <fnt><bib>NT</bib>, <lok>&Luk; 18:15</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="id">bayi</trd>
  </snc>
  <snc mrk="infan.0eto.feto">
    <dif>
      Homa <ref tip="vid" cel="fet.0o">feto</ref>:
      <ekz>
        kiam Elizabeto a&ubreve;dis la saluton de Maria, la <tld/>eto 
        eksaltis en
        &scirc;ia ventro
        <fnt><bib>NT</bib>, <lok>&Luk; 1:41</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="id">janin</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_ib;&c_c;&c_ja;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_l;&c_ju;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>miny&oacute;</trd>,
    <trd>beb&eacute;</trd>,
    <trd>nounat</trd>
  </trdgrp>
  <trd lng="cs">d&ecaron;&tcaron;&aacute;tko</trd>
  <trd lng="de">Kindchen</trd>
  <trd lng="fr">petit<klr tip="amb"> (enfant)</klr></trd>
