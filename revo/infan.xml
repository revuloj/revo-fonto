<drv mrk="infan.0a">
  <kap><tld/>a</kap>
  <snc mrk="infan.0a.deInfano">
    <dif>
      Rilata, apartenanta al <tld/>o:
      <ekz>
        <tld/>a ludo
        <fnt>
          <bib>Fab4</bib>
          <lok>Plumo kaj inkujo</lok>
        </fnt>, vesto, pupo, klaso;
      </ekz>
      <ekz>
        <tld/>aj vestetoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a &ccirc;ambro
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        vo&ccirc;o, kies obtuza kaj serioza sonado prezentis frapantan 
        kontraston kun la malgranda <tld/>a figuro
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la dol&ccirc;aj kisoj de la <tld/>a bu&scirc;o bruligis &scirc;in
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Kinder-</trd>
    <trdgrp lng="hu">
      <trd>gyerek-</trd>,
      <trd>gyermek-</trd>
    </trdgrp>
    <trd lng="nl">kinder-</trd>
  </snc>
  <snc mrk="infan.0a.sensperta">
    <dif>
      Naive sensperta kaj senprudenta; serenanima:
      <ekz>
        <tld/>a argumento
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a animo
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a malseriozeco
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a senzorgeco, naiveco
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>kindlich</trd>,
      <trd>kindisch</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>gyerekes</trd>,
      <trd>gyermekes</trd>,
      <trd>gyermeki</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>ke<ind>kanak</ind>-kanakan</trd>,
      <trd>naif</trd>
    </trdgrp>
    <trd lng="nl">kinderlijk</trd>
    <trd lng="sv">barnslig</trd>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_ib;&c_c;&c_ja;&c_ch;&c_y;</trd>
  <trdgrp lng="ca">
    <trd>infantil</trd>,
    <trd>pueril</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>d&ecaron;tinsk&yacute;</trd>,
    <trd>d&ecaron;tsk&yacute;</trd>
  </trdgrp>
  <trd lng="es">infantil</trd>
  <trdgrp lng="fr">
    <trd>enfantin</trd>,
    <trd>infantile</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>infantile</trd>,
    <trd>bambinesco</trd>
  </trdgrp>
  <trd lng="pl">dzieci&eogonek;cy</trd>
  <trd lng="pt">infantil</trd>
  <trd lng="ro">de <ind>copil</ind></trd>
  <trd lng="ru">&c_d;&c_je;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>detsk&yacute;</trd>,
    <trd>nerozumn&yacute;</trd>,
    <trd>pren: nesk&uacute;sen&yacute;</trd>
  </trdgrp>
  <trd lng="uk">&c_d;&c_i;&c_t;&c_ja;&c_ch;&c_i;&c_j;</trd>
  <trd lng="vo">cilik</trd>
</drv>

<drv mrk="infan.0ajxo">
  <kap>
    <tld/>a&jcirc;o
</kap>
  <snc>
    <dif>
       Ago <tld/>eca, sensrioza, senpripensa:
      <ekz>
        simpla <tld/>a&jcirc;o, kiun seriozaj homoj certe ne permesus al si
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista 
          en Washington en la 15a de a&ubreve;gusto 1910</vrk>
        </fnt>;
      </ekz>
      <ekz>
        malestima nomo de homoj, kiuj okupadis sin je <tld/>a&jcirc;oj
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiu &ccirc;i frazo sonas tiel bele kaj sa&gcirc;e, ke  &gcirc;i 
        <klr>[...]</klr> jam lasas nenian dubon pri tio, ke lingvo arta
        estas <tld/>a&jcirc;o
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiam mi estis <tld/>o, kiel <tld/>o mi parolis, kiel <tld/>o mi
        sentis, 
        kiel <tld/>o mi pensis, 
        nun fari&gcirc;inte plena&gcirc;ulo, mi jam forigis la <tld/>a&jcirc;ojn  
        <fnt>
          <bib>NT</bib>
          <lok>I. Korintanoj 13:11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_ja;&c_c;&c_ib;&c_n;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_l;&c_a;&c_z;&c_je;&c_n;&c_s;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Kinderei</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_i;&c_t;&#x44F;&c_ch;&c_i;&c_j; &c_v;&c_ch;&c_i;&#x43D;&#x43E;&#x43A;</trd>,
    <trd>&c_h;&#x43B;&#x43E;&#x43F;&#8217;&#x44F;&c_c;&c_t;&c_v;&#x43E;</trd>
  </trdgrp>
</drv>

<drv mrk="infan.0eco">
