<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>infan</rad>/o
</kap>
<drv mrk="infan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Homido, rilate al la gepatroj:
      <ekz>
        por patrino ne ekzistas <tld/>o malbela
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>ino 
        <fnt><bib>NT</bib>, <lok>&Mar; 5:40</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Homido kontraste al plenkreskinta homo, &gcirc;is la 12a jaro
      proksimume:
      <ekz>
        en <tld/>o vidi&gcirc;as, kia homo fari&gcirc;os
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        gardejo por <tld/>oj.
      </ekz>
    </dif>
    <ref tip="super" cel="id">ido</ref>
    <refgrp tip="sub">
      <ref cel="knab.0o">knabo</ref>,
      <ref cel="nask">naskito</ref>,
      <ref cel="bub.0o">bubo</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">d&iacute;t&ecaron;</trd>
  <trd lng="en">child</trd>
  <trd lng="fr">enfant</trd>
  <trd lng="es">ni&ntilde;o</trd>
  <trd lng="de">Kind</trd>
  <trd lng="nl">kind</trd>
  <trd lng="pl">dziecko</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_b;&c_jo;&c_n;&c_o;&c_k;</trd>,
    <trd>&c_d;&c_i;&c_t;&c_ja;</trd>
  </trdgrp>
  <trd lng="sv">barn</trd>
</drv>

<drv mrk="infan.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata, apartenanta al <tld/>o:
      <ekz>
        <tld/>a ludo, vesto, pupo, klaso.
      </ekz>
    </dif>
    <trd lng="nl">kinder-</trd>
  </snc>
  <snc>
    <dif>
      Naive sensperta kaj senprudenta:
      <ekz>
        <tld/>a argumento<fnt>Z</fnt>,
      </ekz>
      <ekz>
        senzorgeco.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>kindlich</trd>,
      <trd>kindisch</trd>
    </trdgrp>
    <trd lng="nl">kinderlijk</trd>
    <trd lng="ru">&c_d;&c_je;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>
    <trd lng="sv">barnslig</trd>
  </snc>
  <trd lng="es">infantil</trd>
  <trdgrp lng="fr">
    <trd>enfantin</trd>,
    <trd>infantile</trd>
  </trdgrp>
</drv>

<drv mrk="infan.0istino">
  <kap><tld/>istino</kap>
  <snc>
    <ref tip="dif" cel="vart.0istino">vartistino.</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Kinderg&auml;rtnerin</trd>,
    <trd>Erzieherin</trd>
  </trdgrp>
  <trd lng="nl">kinderverzorgster</trd>
</drv>

<drv mrk="infan.mir0o">
  <kap>mir<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="I"/>o havanta mirigajn kapablojn:
      <ekz>
        Mozarto estis vera mir<tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Wunderkind</trd>
  <trdgrp lng="fr">
    <trd>enfant prodige</trd>,
    <trd>enfant <ind>surdou&eacute;</ind></trd>
  </trdgrp>
  <trd lng="nl">wonderkind</trd>
  <trd lng="sv">underbarn</trd>
  <trd lng="ru">&c_v;&c_u;&c_n;&c_d;&c_je;&c_r;&c_k;&c_i;&c_n;&c_d;</trd>
</drv>

<drv mrk="infan.sucx0o">
  <kap>su&ccirc;<tld/>o</kap>
  <snc>
    <dif>
      Juna <ref tip="super" cel="infan.0o"><tld/>o</ref> ankora&ubreve; mamnutrata:
      <ekz>
        <uzo tip="stl">FIG</uzo> la su&ccirc;<tld/>a periodo
        de nia lingvo
        <fnt>(<aut>Zamenhof</aut>: <vrk>Parolo anta&ubreve;
        la Kvara Kongreso Esperantista en Dresden 1908</vrk>)</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>S&auml;ugling</trd>,
    <trd>Baby</trd>
  </trdgrp>
  <trd lng="en">baby</trd>
  <trd lng="es">lactante</trd>
  <trdgrp lng="fr">
    <trd>b&eacute;b&eacute;</trd>,
    <trd>nourrisson</trd>
  </trdgrp>
  <trd lng="nl">zuigeling</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_u;&c_d;&c_n;&c_o;&c_j; &c_r;&c_je;&c_b;&c_jo;&c_n;&c_o;&c_k;</trd>,
    <trd>&c_m;&c_l;&c_a;&c_d;&c_je;&c_n;&c_je;&c_c;</trd>
  </trdgrp>
</drv>
</art>
