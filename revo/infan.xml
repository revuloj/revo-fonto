<drv mrk="infan.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="infan.0eto.bebo">
    <dif>
      Malgranda, juna infano, <ref tip="vid" cel="et.0ulo">etulo</ref>;
      <ref tip="sin" cel="beb.0o">bebo</ref>:
      <ekz>
        oni venigis al li anka&ubreve; siajn <tld/>etojn, por ke li
        tu&scirc;u ilin
        <fnt><bib>NT</bib>, <lok>&Luk; 18:15</lok></fnt>;
      </ekz>
      <ekz>
        vi trovos <tld/>eton, &ccirc;irka&ubreve;vinditan kaj ku&scirc;antan en staltrogo
        <fnt>
          <bib>NT</bib>
          <lok>Luko 2:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        pala kaj malforta li estis ja kiel <tld/>eto
        <fnt>
          <bib>VivZam</bib>
          <lok>Vivo de Zamenhof</lok>
        </fnt>.
      </ekz>
