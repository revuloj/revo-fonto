<drv mrk="infan.0a">
  <kap><tld/>a</kap>
  <snc mrk="infan.0a.deInfano">
    <dif>
      Rilata, apartenanta al <tld/>o:
      <ekz>
        <tld/>a ludo
        <fnt>
          <bib>Fab4</bib>
          <lok>Plumo kaj inkujo</lok>
        </fnt>, vesto, pupo, klaso;
      </ekz>
      <ekz>
        <tld/>aj vestetoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la rakontoj <klr>[&#x2026;]</klr> pla&ccirc;as al <tld/>aj legantoj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Anta&ubreve;parolo de la tradukinto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a &ccirc;ambro
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        vo&ccirc;o, kies obtuza kaj serioza sonado prezentis frapantan
        kontraston kun la malgranda <tld/>a figuro
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la dol&ccirc;aj kisoj de la <tld/>a bu&scirc;o bruligis &scirc;in
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Kinder-</trd>
    <trdgrp lng="hu">
      <trd>gyerek-</trd>,
      <trd>gyermek-</trd>
    </trdgrp>
    <trd lng="nl">kinder-</trd>
  </snc>
  <snc mrk="infan.0a.sensperta">
    <dif>
      Naive sensperta kaj senprudenta; serenanima:
      <ekz>
        <tld/>a argumento
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a animo
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a malseriozeco
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a senzorgeco, naiveco
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>kindlich</trd>,
      <trd>kindisch</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>gyerekes</trd>,
      <trd>gyermekes</trd>,
      <trd>gyermeki</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>ke<ind>kanak</ind>-kanakan</trd>,
      <trd>naif</trd>
    </trdgrp>
    <trd lng="nl">kinderlijk</trd>
    <trd lng="sv">barnslig</trd>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_ib;&c_c;&c_ja;&c_ch;&c_y;</trd>
  <trdgrp lng="ca">
    <trd>infantil</trd>,
    <trd>pueril</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>d&ecaron;tinsk&yacute;</trd>,
    <trd>d&ecaron;tsk&yacute;</trd>
  </trdgrp>
  <trd lng="es">infantil</trd>
  <trdgrp lng="fr">
