<drv mrk="infan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="infan.0o.juna">
    <dif>
      <ref tip="sin" cel="hom.0ido.infano">Homido</ref> kontraste al
      plenkreskinta homo, &gcirc;is la 12a jaro proksimume:
      <ekz>
        <tld/>o ne estas matura homo
        <fnt>
          <bib>F</bib>
          <lok>&FE; 6</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o ploras, &ccirc;ar &gcirc;i volas man&gcirc;i
        <fnt><bib>F</bib>, <lok>&FE; 16</lok></fnt>;
      </ekz>
      <ekz>
        ludejo por <tld/>oj
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 12 (110)</lok>
        </fnt>;
      </ekz>
      <ekz>
        en <tld/>o vidi&gcirc;as, kia homo fari&gcirc;os
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="bub.0o">bubo</ref>,
      <ref cel="knab.0o">knabo</ref>,
    </refgrp>
    <ref tip="vid" cel="jun.0ulo">junulo</ref>
  </snc>
  <snc mrk="infan.0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="dif" cel="id.0o.GEN">Ido<sncref/></ref>:
      <ekz>
        li havas dek unu <tld/>ojn
        <fnt><bib>F</bib>, <lok>&FE; 12</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>o mia, kion vi timas <klr>[&#x2026;]</klr>
        <fnt>
          <bib>ElektFab</bib>
          <lok>Re&gcirc;o rano a&ubreve; fera Henriko</lok>
        </fnt>?
      </ekz>
      <ekz>
        li estis libera homo, jam delonge vidvo kaj anka&ubreve; liaj <tld/>oj jam malaperis
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>;
      </ekz>
      <ekz>
        en doloro vi naskados <tld/>ojn
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 3:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        li kondukis la patron de la <tld/>ino
        <fnt><bib>NT</bib>, <lok>&Mar; 5:40</lok></fnt>;
      </ekz>
      <ekz>
        por patrino ne ekzistas <tld/>o malbela
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
