<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap><ofc>3</ofc><rad>adapt</rad>/i</kap>
<drv mrk="adapt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="adapt.0i.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Ta&ubreve;ge, &gcirc;uste kunigi ion materian al io:
      <ekz><tld/>i tenilon al vazo, seruron al pordo.</ekz>
    </dif>
    <trdgrp lng="en">
      <trd>adapt</trd>,
      <trd>fit</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>adaptar</trd>,
      <trd>ajustar</trd>,
      <trd>ajeitar</trd>,
      <trd>acomodar</trd>
    </trdgrp>
  </snc>
  <snc mrk="adapt.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Konformigi por specialaj celoj a&ubreve; kondi&ccirc;oj:
      <ekz>
        oni devas
        <tld/>i sian parolon al la  cirkonstancoj;
      </ekz><ekz>
        tiuj versoj estas malbone <tld/>itaj al la muziko.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>adapt</trd>,
      <trd>adjust</trd>
    </trdgrp>
    <trd lng="sv">anpassa</trd>
  </snc>
  <refgrp tip="vid">
    <ref cel="far.al0i">alfari</ref>,
    <ref cel="gxust.al0igi">al&gcirc;ustigi</ref>,
    <ref cel="akomod.0i">akomodi</ref>.
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_l;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>, 
    <trd>&c_p;&c_r;&c_y;&c_s;&c_t;&c_a;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">anpassen</trd>
  <trd lng="es">adaptar</trd>
  <trdgrp lng="fr">
    <trd>adapter</trd>,
    <trd>assortir</trd>,
    <trd>ajuster</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>adapt&aacute;l</trd>,
    <trd>alkalmass&aacute; tesz</trd>,
    <trd>hozz&aacute;igaz&iacute;t</trd>
  </trdgrp>
  <trd lng="it">adattare</trd>
  <trd lng="nl">aanpassen</trd>
  <trdgrp lng="pt">
    <trd>adaptar</trd>,
    <trd>ajustar</trd>,
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_s;&c_p;&c_o;&c_s;&c_a;&c_b;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_l;&c_a;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_g;&c_o;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="adapt.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="arangx.0ajxo.TEK">Aran&gcirc;a&jcirc;o</ref>
      destinita por ebligi kunan laboron de diversaj aparatoj,
      cirkvitoj, <ref cel="organ.0o.TEK">organoj</ref> de
      ma&scirc;ino a&ubreve; komputilo ktp:
      <ekz>ekrana <tld/>ilo SVGA.</ekz>
    </dif>
    <rim>
      <tld lit="A"/>ilo estas pli inteligenta
      <ref tip="super" cel="fac.inter0o">interfaco</ref> ol simpla
      <ref cel="junt.0o.ilo">junto</ref>,
      <ref cel="konver3.0i.KOMP">konvertanta</ref> signalojn
      a&ubreve; adaptanta fizikajn parametrojn de la kunigataj
      aparatoj.
    </rim>
  </snc>
  <trdgrp lng="en">
    <trd>adapter</trd>,
    <trd>controller</trd>
  </trdgrp>
  <trd lng="es">adaptador</trd>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_a;&c_p;&c_t;&c_a;&c_r;</trd>, 
    <!-- &c_a;&c_d;&c_a;&c_p;&c_t;&c_e;&c_r;? -->
    <trd>&c_k;&c_a;&c_n;&c_t;&c_r;&c_o;&c_l;&c_je;&c_r;</trd>
  </trdgrp>
  <trd lng="hu">adapter</trd>
  <trd lng="nl">adapter</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_d;&c_a;&c_p;&c_t;&c_je;&c_r;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_t;&c_r;&c_o;&c_l;&c_l;&c_je;&c_r;</trd>
  </trdgrp>
</drv>
</art>
