<vortaro>
<art mrk="$Id: o1.xml,v 1.8 2021/05/09 13:53:16 revo Exp $">
<kap>
    <ofc>*</ofc> <ofc>7</ofc><rad>o</rad>
</kap>
<drv mrk="o1.0">
  <kap><tld/></kap>
    <uzo tip="fak">LIN</uzo>
    <uzo tip="fak">GRA</uzo>
    <uzo tip="fak">FON</uzo>
  <snc mrk="o1.0.vokalo">
     <dif>
      La dekna&ubreve;a <ref tip="super" cel="liter.0o.propra">litero<sncref/></ref>
      de la Esperanta alfabeto      
      <ekz>
        la &leftquot;o&rightquot; devas esti parolata meze-longe
        <fnt>
          <bib>LR</bib>
        </fnt>,
      </ekz> uzata i.a. kiel:
     </dif>
     <subsnc>
       <uzo tip="fak">KEM</uzo>
       <dif>
       (maj.) simbolo de oksigeno: <ekz> <tld lit="O"/>.</ekz>
       </dif>
       <trd lng="fr">oxyg&egrave;ne</trd>
     </subsnc>
     <subsnc>
       <uzo tip="fak">TEK</uzo>
       <dif>
       (maj.) signo de formo: <ekz> <tld lit="O"/>-aperturo.</ekz>
       </dif>
       <trd lng="fr">O<klr> (forme de)</klr></trd>
     </subsnc>   
     <trd lng="fr">o<klr> (lettre)</klr></trd>
  </snc>
  <ref tip="hom" cel="o.0">-<tld/> (fina&jcirc;o)</ref>
  <ref tip="vid" cel="ho.0">ho, oh</ref>
</drv>
</art>

