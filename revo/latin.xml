<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Latin</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="latin.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">HIS</uzo>
  <uzo tip="fak">LIN</uzo>
  <snc>
    <dif>
      Lingvo, en kiu parolis la antikvaj
      <ref tip="vid" cel="romi.0ano">Romianoj</ref>,
      kaj kiu hodia&ubreve; ankora&ubreve; estas uzata de la
      <ref tip="vid" cel="katoli.rom0a">Romkatolika</ref> eklezio:
      <ekz>
        venis fino al mia <tld lit="l"/>o<fnt>Z</fnt>
        <klr>(el&ccirc;erpi&gcirc;is mia tuta scio)</klr>;
      </ekz>
      <ekz>
        <tld lit="l"/>igi tekston.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">Latin</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_t;&c_y;&c_n;&c_mol;</trd>,
    <trd>&c_l;&c_a;&c_t;&c_i;&c_n;&c_s;&c_k;&c_i;&c_j; &c_ja;&c_z;&c_y;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="latin.0ida">
  <kap>
    <tld lit="l"/>idaj lingvoj
  </kap>
  <uzo tip="fak">LIN</uzo>
  <snc>
    <dif>
      Grupo de parencaj
      <ref tip="super" cel="hind.0euxropa">hinde&ubreve;ropaj</ref> lingvoj,
      devenintaj el <tld/>o:
      la itala, la hispana, la portugala, la franca kaj aliaj.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>Romance languages</trd>,
    <trd>Romanic languages</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_o;&c_m;&c_a;&c_n;&c_s;&c_k;&c_i;&c_je; &c_ja;&c_z;&c_y;&c_k;&c_i;</trd>
</drv>

</art>

<!--
$Log$
-->
</vortaro>