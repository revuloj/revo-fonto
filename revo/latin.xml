<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: latin.xml,v 1.28 2024/11/23 19:22:18 revo Exp $">
    <kap><ofc>10</ofc>
      <rad>Latin</rad>/o
      <fnt><bib>JED</bib></fnt>
    </kap>

<drv mrk="latin.0o">
  <kap><ofc>10</ofc><tld/>o</kap>
  <snc mrk="latin.0o.LIN">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="lst" cel="hind.0euxropa_lingvo" lst="voko:lingvoj">Hinde&ubreve;ropa lingvo</ref>,
      en kiu parolis antikvaj <ref cel="lati.0ano">latianoj</ref>,
      poste la romanoj kaj la okcidentaj
      <ref tip="vid" cel="romi.0ano">romianoj</ref>;
      plu uzata interalie de la
      <ref tip="vid" cel="katoli.rom0a">Romkatolika</ref>
      eklezio:
      <ekz>
        &scirc;ajnas, ke Izidoro de Sevilo (m. 636) unua proklamis
        <tld/>on
        <ctl>sankta lingvo</ctl>;
        <fnt>
          <aut>S. Pokrovskij</aut>: <vrk>Slavonaj skriboj</vrk>.
          <bib>LOdE</bib>, <lok>1999:2 (n-ro 52), p. 13a</lok>
        </fnt>;
      </ekz><ekz>
        la tradicio esperantigi <tld lit="l"/>ajn nomojn pruvas,
        ke, kiel dirus <nom>Neves</nom>, Esperanto estas nepinjo de <tld/>o
        <fnt>
          <aut>Jorge CAMACHO</aut>: <vrk>Pretas jen
          monument'!</vrk>.
          <bib>Monato</bib>
        </fnt>;
      </ekz><ekz>
        li bone sciis ofendi neniun: li ridis la <tld/>on de la
        seminarioj kaj defendis &ccirc;e alia okazo sian episkopon
        <fnt>
          <aut>I. Turgenjev, tr. Kabe</aut>:
          <vrk>Patroj kaj filoj</vrk>.
          <lok>Inko Tyres&ouml; 2001. &#8208; &Ccirc;ap. 21a.</lok>
        </fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        Esperanto estas la <tld lit="l"/>o de la demokratio;
      </ekz><ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>venis fino al mia <tld lit="l"/>o</ind>
        <klr>(el&ccirc;erpi&gcirc;is mia tuta scio)</klr>
        <trd lng="fr">je donne ma langue au chat</trd>
        <trd lng="nl">ik ben aan het eind van mijn
        <ind>Latijn</ind></trd>
        <fnt><bib>PrV</bib></fnt>;
      </ekz><ekz>
        <tld lit="l"/>igi tekston.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="lati.0o.HIS">Latio<sncref/></ref>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_c;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_l;&c_a;&c_c;&c_ib;&c_n;&c_s;&c_k;&c_a;&c_ja; &c_m;&c_o;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_l;&c_a;&c_t;&c_i;&c_n;&c_s;&c_k;&c_i; &c_je;&c_z;&c_i;&c_k;</trd>
  <trd lng="de">Latein</trd>
  <trd lng="br">latin</trd>
  <trd lng="el">&lambda;&alfa;&tau;&jota;&ny;&jota;&kappa;&alfa_ton;</trd>
  <trd lng="en">Latin</trd>
  <trd lng="fr">latin</trd>
  <trd lng="hu">latin</trd>
  <trd lng="id">Latin</trd>
  <trd lng="nl">Latijn</trd>
  <trd lng="pl">&lstroke;acina</trd>
  <trd lng="pt">latim</trd>
  <trd lng="ro">limba <ind>latin&abreve;</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_t;&c_y;&c_n;&c_mol;</trd>,
    <trd>&c_l;&c_a;&c_t;&c_i;&c_n;&c_s;&c_k;&c_i;&c_j; &c_ja;&c_z;&c_y;&c_k;</trd>
  </trdgrp>
  <trd lng="zh">&#25289;&#19969; <pr>l&amacron;d&imacron;ng</pr></trd>
</drv>

<drv mrk="latin.0a">
  <kap><ofc>10</ofc>
    <tld lit="l"/>a
  </kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        la helena kaj <tld lit="l"/>a kulturoj estis tiel brilaj,
        ke la influo de la respondaj lingvoj &scirc;vebis sur la tuta E&ubreve;ropo
        dum du jarmiloj
        <fnt>
	<aut>Andr&eacute; Cherpillod</aut>
	<vrk><url ref="&Monato;/2000/007059.php">La influo de la hebrea lingvo sur Esperanto</url></vrk>
	<bib>Monato</bib>
	<lok>2000/12, p. 18-19</lok>
        </fnt>;
      </ekz><ekz>
        nenia decido e&ccirc; de plej a&ubreve;toritata kongreso neniam povas jam
        doni al la lingvo <tld lit="l"/>a tiun forton, kiun &gcirc;i havis en la
        mezaj centjaroj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz><ekz>
        a&ubreve; elekti ian el la ekzistantaj lingvoj vivantaj, a&ubreve; elekti
        ian el la lingvoj mortintaj (ekzemple <tld/>an, grekan,
        hebrean)<klr>[&#8230;]</klr>
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz><ekz>
        la plej granda parto de la lingvoj kiuj uzas la alfabeton <tld lit="l"/>an
        havas ankora&ubreve; amason da aliaj literoj kun signetoj superliniaj
        kaj subliniaj
        <fnt>
          <bib>LR</bib>
          <lok>1. Alfabeto</lok>
        </fnt>;
      </ekz><ekz>
        li tradukis la nomon de la skarabo en la lingvon <tld lit="l"/>an
        kaj priskribis <klr>[&#8230;]</klr> &gcirc;ian naturon
        <fnt>
          <bib>Fab4</bib>
          <lok>Sterkskarabo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_c;&c_ib;&c_n;&c_s;&c_k;&c_ib;</trd>
  <trd lng="de">lateinisch</trd>
  <trd lng="nl">Latijns</trd>
  <trd lng="pl">&lstroke;aci&nacute;ski</trd>
  <trd lng="ro">latin</trd>
  <trd lng="zh">&#25289;&#19969; <pr>l&amacron;d&imacron;ng</pr></trd>
</drv>

<drv mrk="latin.0idaj_lingvoj">
  <kap><ofc>10</ofc><tld lit="l"/>idaj lingvoj,
  <var><kap>romidaj lingvoj</kap></var></kap>
  <snc mrk="latin.0idaj_lingvoj.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      Grupo da parencaj
      <ref cel="hind.0euxropa_lingvo">hinde&ubreve;ropaj lingvoj</ref>, devenintaj el <tld/>o:
      la itala, la hispana, la portugala, la franca, la rumana kaj
      aliaj:
      <ekz>
         la&ubreve;sone Esperanto similas la plej sudajn <tld lit="l"/>idajn
         lingvojn
        <fnt>
          <aut>E. Privat</aut>:
          <vrk>Esprimo de sentoj en Esperanto</vrk>
          <lok>&Gcirc;enevo, 1931. &Ccirc;ap. 2-a.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>von Wahl</nom> <klr>[&#8230;]</klr> celis difini derivregulojn,
        kiuj ebligos ricevi &ccirc;iujn internaciajn vortojn en la formo,
        kiun donis al ili la romidaj lingvoj
        <fnt>
          <aut>G. Waringhien</aut>
          <vrk><url ref="https://bibliotekoj.org/literaturamondo/PDF/1948/LM%201948-0304c.pdf">
          Esperanto anta&ubreve; la tribunalo</url></vrk>
          <lok>Literatura Mondo, 1948-03 &gcirc;is 04, p.39a</lok>
        </fnt>;
      </ekz>
      <ekz>
        temas pri verba interrespondado de tensoj, kiu en la
        <tld lit="l"/>idaj lingvoj estas iom komplika afero kompare
        kun la simpla esperanta strukturo
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj'</aut>
          <vrk><url ref="&Monato;/2014/011154.php">Kerno de la itala
          lingvo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la germana estas &gcirc;ermanida lingvo; la franca kaj itala estas
        <tld lit="l"/>idaj lingvoj, do proksime parencaj inter si, sed
        sen reciproka komprenebleco
        <fnt>
          <bib>Monato</bib>
          <aut>Mireille Grosjean</aut>
          <vrk><url ref="&Monato;/2011/010267.php">Svislando:
           la arto interkompreni&gcirc;i</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj <tld lit="l"/>idaj lingvoj originis de la popola <tld lit="l"/>o
        jam en la historia epoko, post la disfalo de la romia imperio
        <fnt>
          <bib>Monato</bib>
          <aut>Mihail Korotkov</aut>
          <vrk><url ref="&Monato;/2011/010218.php">La origino
           de la homaj lingvoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_m;&c_a;&c_n;&c_s;&c_k;&c_ib;&c_ja; &c_m;&c_o;&c_v;&c_y;</trd>
  <trdgrp lng="br">
    <trd>yezho&ugrave; <ind>latin</ind></trd>,
    <trd>yezho&ugrave; <ind>romanek</ind></trd>
  </trdgrp>
  <trd lng="de">romanische Sprachen</trd>
  <trdgrp lng="en">
    <trd>Romance languages</trd>,
    <trd>Romanic languages</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>langues n&eacute;olatines</trd>,
    <trd>langues romanes</trd>
  </trdgrp>
  <trd lng="hu">latin nyelvek</trd>
  <trd lng="id"><ind>bahasa</ind> Romanik</trd>
  <trd lng="nl">Romaanse talen</trd>
  <trd lng="pl"> j&eogonek;zyki <ind>roma&nacute;skie</ind></trd>
  <trd lng="pt">l&iacute;nguas latinas</trd>
  <trd lng="ro">limbi <ind>romanice</ind></trd>
  <trd lng="ru">&c_r;&c_o;&c_m;&c_a;&c_n;&c_s;&c_k;&c_i;&c_je; &c_ja;&c_z;&c_y;&c_k;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#32599;&#35821; <pr>Lu&oacute;y&#468;</pr></trd>,
    <trd>&#32645;&#35486; <pr>Lu&oacute;y&#468;</pr></trd>,
    <trd>&#32599;&#26364;&#35821;&#26063; <pr>Lu&oacute;m&agrave;ny&#468;z&uacute;</pr></trd>,
    <trd>&#32645;&#26364;&#35486;&#26063; <pr>Lu&oacute;m&agrave;ny&#468;z&uacute;</pr></trd>
  </trdgrp>
</drv>
