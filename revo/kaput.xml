  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_a;&c_m;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_b;&c_ib;&c_t;&c_y;</trd>
