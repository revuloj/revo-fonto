</drv>

<drv mrk="kaput.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="kaput.0igi.FAMILIARE">
    <uzo tip="klr">(familiare)</uzo>
    <dif>
      <ref tip="dif" cel="difekt.0i">Difekti</ref>,
      <ref tip="sin" cel="detru.0i">detrui</ref>:
      <ekz>
        <nom>Greetje van Zeitgeist</nom> penis refunkciigi la
        kafaparaton kiun <tld/>igis &jcirc;usa estrarkunsido
        de <nom>TUJO</nom>
        <fnt><aut>I. Ertl</aut>, <vrk>La Postdomo</vrk>,
        <lok>2003</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_a;&c_m;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_b;&c_u;&c_r;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>espatllar</trd>,
    <trd>malmetre</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><ind>kaputt</ind> machen</trd>,
    <trd>zerst&ouml;ren</trd>
  </trdgrp>
  <trd lng="es">romper</trd>
  <trd lng="fr">d&eacute;glinguer</trd>
  <trd lng="pl">uszkadza&cacute;</trd>
  <trd lng="pt">escangalhar</trd>
  <trd lng="ro">deteriora</trd>
  <trdgrp lng="zh">
    <trd>&#27584;&#25613; <pr>hu&#464;s&#468;n</pr></trd>,
    <trd>&#27584;&#28357; <pr>hu&#464;mi&#232;</pr></trd>,
    <trd>&#27584;&#22750; <pr>hu&#464;h&#224;i</pr></trd>
  </trdgrp>
</drv>
</art>

