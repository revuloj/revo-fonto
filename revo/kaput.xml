  <snc mrk="kaput.0a.FAMILIARE">
    <uzo tip="klr">(familiare)</uzo>
    <dif>
      Difektita, ne funkcianta:
      <ekz>
        kiel ma&scirc;ino <klr>...</klr> balda&ubreve; <tld/>a
        li pasas anta&ubreve;en pli kaj pli malrapide
        <fnt>
          <aut>M&oacute;ricz Z., trad. Vizi L.</aut>,
          <vrk><url
          ref="http://www.ipernity.com/blog/eszperanto/393660">
          La lasta bandito</url></vrk>,
          <lok>2012-06-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>espatllat</trd>,
    <trd>malm&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="fr">
