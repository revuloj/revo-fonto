  <trdgrp lng="zh">
    <trd>&#x4E0D;&#x53EF;&#29992; <pr>b&#xF9;k&#x11B;y&#xF2;ng</pr></trd>,
    <trd>&#x574F;&#x4E86; <pr>hu&#xE0;ile</pr></trd>,
    <trd>&#x574F;&#25481; <pr>hu&#xE0;idi&#xE0;o</pr></trd>,
