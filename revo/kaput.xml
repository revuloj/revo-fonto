  </trdgrp>
  <trd lng="de">kaputt</trd>
  <trdgrp lng="fr">
    <trd>amoch&eacute;</trd>,
    <trd>d&eacute;glingu&eacute;</trd>
  </trdgrp>
  <trd lng="pl">zniszczony</trd>
  <trdgrp lng="pt">
    <trd>escagalhado</trd>,
    <trd>com defeito</trd>,
    <trd>n&atilde;o us&aacute;vel por desarranjo</trd>
  </trdgrp>
  <trd lng="ro">distrus</trd>
  <trdgrp lng="sk">
    <trd>doriaden&yacute;</trd>,
