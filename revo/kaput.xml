    <trd>&#1087;&#1086;&c_b;&c_i;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_z;&#1085;&c_i;&c_shch;&c_je;&#1085;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#19981;&#21487;&#29992; <pr>b&#249;k&#283;y&#242;ng</pr></trd>,
    <trd>&#22351;&#20102; <pr>hu&#224;ile</pr></trd>,
    <trd>&#22351;&#25481; <pr>hu&#224;idi&#224;o</pr></trd>,
    <trd>&#31934;&#30130;&#21147;&#31469; <pr>j&#299;ngp&#237;l&#236;ji&#233;</pr></trd>
