<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Jenisej</rad>/o
    </kap>

    <drv mrk="jenisej.J0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
	<dif>
	  La plej akvori&ccirc;a rivero de
	  <ref tip="malprt" cel="rus.R0ujo">Rusujo</ref>, 4102km longa,
	  fluanta norden, de <ref cel="sajan.0o">Sajano</ref> en la
	  <ref cel="kara.0aMaro">Karaan maron</ref> kaj formanta limon
	  inter la <ref tip="malprt" cel="siberi.0o">Siberio</ref> Okcidenta
	  kaj la Siberio Orienta.
	</dif>
      </snc>
      <trd lng="de">Jenissej</trd>
      <trd lng="fr">Ienisse&iuml;</trd>
      <trd lng="ru">&c_Je;&c_n;&c_i;&c_s;&c_je;&c_j;</trd>
    </drv>

    <drv mrk="jenisej.J0aGolfo">
      <kap><tld/>a Golfo</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
	<dif>
	  Golfo en la <ref tip="malprt" cel="kara.0aMaro">Karaa maro</ref>
	  kiun formas elfluejo <tld/>o.
	</dif>
      </snc>
      <trd lng="de">Jenissej-Busen</trd>
      <trd lng="ru">&c_Je;&c_n;&c_i;&c_s;&c_je;&c_j;&c_s;&c_k;&c_i;&c_j; &c_z;&c_a;&c_l;&c_i;&c_v;</trd>
    </drv>

  </art>

