<art mrk="$Id: kanoni1.xml,v 1.24 2025/11/11 06:45:15 revo Exp $">
<kap>
  <rad>kanoniz</rad>/i
  <fnt><bib>VdE</bib></fnt>
</kap>

<drv mrk="kanoni1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="dif" cel="kanon.en0igi">Enkanonigi</ref>:
      <ekz>
        la eklezio <tld/>is nur 86 el la &gcirc;isnunaj 265 papoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2012/010367.php">Organdonanto jes,
          organdonanto ne</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      Estas nenia motivo uzi tiun formon, malsimilan al la normala
      esperanta <ctl>enlistigi</ctl> ktp.  La&ubreve; la 15a Regulo oni
      preferu <ctl>enkanonigi</ctl>.
      <aut>Sergio Pokrovskij</aut>
    </rim>
    <rim>
      La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>deklari sankta</ke>, <ke>sanktuligi</ke>, <ke>sanktigi</ke>.
    </rim>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>kanonizovat</trd>,
    <trd>prohl&aacute;sit za svat&eacute;ho</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>kanonisieren</trd>,
    <trd>heiligsprechen</trd>
  </trdgrp>
  <trd lng="es">canonizar</trd>
  <trd lng="fr">canoniser</trd>
  <trdgrp lng="hu">
    <trd>szentt&eacute; avat</trd>,
    <trd>kanoniz&aacute;l</trd>
  </trdgrp>
  <trd lng="ja">&#x8056;&#x5217;&#x306B;&#x52A0;&#x3048;&#x308B;  <pr>&#x3072;&#x3058;&#x308A;&#x308C;&#x3064;&#x306B;&#x304F;&#x308F;&#x3048;&#x308B;</pr></trd>
  <trdgrp lng="nl">
    <trd>canoniseren<klr tip="ind"> (ontr.)</klr></trd>,
    <trd>heilig verklaren<klr tip="ind"> (ontr.)</klr></trd>,
    <trd>zalig verklaren<klr tip="ind"> (ontr.)</klr></trd>
  </trdgrp>
  <trd lng="pl">kanonizowa&cacute;</trd>
  <trdgrp lng="ro">
    <trd>canoniza</trd>,
    <trd>declara ca sf&acirc;nt</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_n;&c_o;&c_n;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">kanonizova&tcaron;</trd>
  <trd lng="uk">&c_k;&c_a;&c_n;&c_o;&c_n;&c_ib;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x4E0A;&#x5929;&#x5802; <pr>sh&agrave;ngti&amacron;nt&aacute;ng</pr></trd>,
    <trd>&#x5FB7;&#x9AD8; <pr>d&eacute;g&amacron;o</pr></trd>
  </trdgrp>
</drv>
</art>
