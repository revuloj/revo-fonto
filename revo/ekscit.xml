  <trd lng="ka">&#x10D0;&#x10E6;&#x10E3;&#x10EB;&#x10E0;&#x10D0;&#x10D5;&#x10D4;&#x10DC;</trd>
  <trd lng="km">&#x179A;&#x17C6;&#x1797;&#x17BE;&#x1794;</trd>
  <trd lng="ko">&#xD765;&#xBD84;</trd>
  <trd lng="lo">&#xE95;&#xEB7;&#xEC8;&#xE99;&#xEC0;&#xE95;&#xEB1;&#xEC9;&#xE99;</trd>
  <trd lng="ml">&#xD09;&#xD23;&#xD30;&#xD4D;&#xD24;&#xD4D;&#xD24;&#xD41;&#xD15;</trd>
  <trd lng="mr">&#x91C;&#x93E;&#x917;&#x943;&#x924;</trd>
  <trd lng="ne">&#x909;&#x924;&#x94D;&#x924;&#x947;&#x91C;&#x93F;&#x924;</trd>
  <trd lng="ny">chimwemwe</trd>
  <trd lng="pa">&#xA09;&#xA24;&#xA38;&#xA3E;&#xA39;&#xA3F;&#xA24;</trd>
  <trd lng="ps">&#x648;&#x647;&#x685;&#x648;&#x6CC;</trd>
  <trd lng="pt">excitar</trd>
  <trd lng="ru">&c_v;&c_o;&c_z;&c_b;&c_u;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>
