<drv mrk="ekscit.0o">
  <kap>
    <tld/>o, <var><kap><tld/>eco</kap></var>
  </kap>
  <snc mrk="ekscit.0o.stato">
    <dif>
      <ref tip="super" cel="stat.0o.atributa">Stato</ref>
      de iu a&ubreve; io <tld/>ita:
      <ekz>
        <tld/>o anta&ubreve; <ref cel="rubikon.0o">Rubikono</ref>
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;arma tono de &gcirc;oja <tld/>iteco jam regis en la tuta domo
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        regis granda konfuzo kaj <tld/>o, sed sen celo kaj plano
        <fnt>
          <bib>BdV</bib>
        </fnt>.
      </ekz>
    </dif>    
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_z;&c_b;&c_u;&c_d;&c_zh;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_z;&c_r;&c_u;&c_sh;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">excitaci&oacute;</trd>
  <trd lng="cs">dr&aacute;&zcaron;divost</trd>
  <trdgrp lng="de">
    <trd>Aufregung</trd>,
    <trd>Erregtheit</trd>
  </trdgrp>
  <trd lng="es">excitaci&oacute;n</trd>
  <trd lng="fr">excitation<klr tip="ind"> (&eacute;tat)</klr></trd>
  <trd lng="he">&he;&tav;&resh;&gimel;&shin;&vav;&tav;</trd>
  <trd lng="hu">izgatotts&aacute;g</trd>
  <trdgrp lng="pl">
    <trd>ekscytacja</trd>,
    <trd>podniecenie</trd>,
    <trd>wzburzenie</trd>,
    <trd>zdenerowanie</trd>,
    <trd>irytacja</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_z;&c_b;&c_u;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>pob&uacute;renie</trd>,
    <trd>podr&aacute;&zcaron;denie</trd>,
    <trd>vzru&scaron;enie</trd>
  </trdgrp>
</drv>

<drv mrk="ekscit.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ekscit.0igxi.PSI">
    <dif>
      I&gcirc;i <tld/>ita, malkvieti&gcirc;i:
      <ekz>
        li estas tre ekkolerema, kaj
        <tld/>i&gcirc;as &ccirc;e la plej malgranda
        bagatelo
        <fnt>
          <bib>F</bib>
          <lok>&FE; 41</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;is kolero kontra&ubreve; li
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 32:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tuta urbo <tld/>i&gcirc;is, kaj la popolo kunkuris
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 21:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>i&gcirc;as kiel bolanta lakto
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_z;&c_b;&c_u;&c_d;&c_zh;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_u;&c_z;&c_r;&c_u;&c_sh;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">excitar-se</trd>
  <trd lng="de">sich <ind>erregen</ind></trd>
  <trd lng="es">excitarse</trd>
  <trd lng="fr">s'<ind>exciter</ind></trd>
  <trd lng="he">&lamed;&he;&tav;&resh;&gimel;&shin;</trd>
  <trdgrp lng="hu">
    <trd>felizgatja mag&aacute;t</trd>,
    <trd>felindul</trd>,
    <trd>begerjed</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ekscytowa&cacute; si&eogonek;</trd>,
    <trd>podnieca&cacute; si&eogonek;</trd>,
    <trd>wzburzy&cacute; si&eogonek;</trd>,
    <trd>denerwowa&cacute; si&eogonek;</trd>,
    <trd>irytowa&cacute; si&eogonek;</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_z;&c_b;&c_u;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="ekscit.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc mrk="ekscit.0igxo.PSI">
    <dif>
      Malkvieti&gcirc;o, forta vigli&gcirc;o:
      <ekz>
        kiam tagi&gcirc;is, fari&gcirc;is inter la soldatoj ne malgranda <tld/>i&gcirc;o, kio fari&gcirc;is kun
        Petro
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 12:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        li pali&gcirc;is de la <tld/>i&gcirc;o
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia<klr>[j&#x2026;]</klr> okuloj brilis de forta <tld/>i&gcirc;o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        perdinte la spiron pro granda <tld/>i&gcirc;o, li devis ekspiri pli profunde
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve; mia propra spino kuretis <tld/>i&gcirc;o
        <fnt><aut>S. &Scaron;timec</aut>, <vrk>Ombro sur interna
        pejza&gcirc;o</vrk>, <lok>1984</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">excitaci&oacute;</trd>
  <trd lng="fr">excitation<klr> (changement d'&eacute;tat)</klr></trd>
  <trdgrp lng="pl">
    <trd>ekscytacja</trd>,
    <trd>podniecenie</trd>,
    <trd>wzburzenie</trd>,
    <trd>zdenerowanie</trd>,
    <trd>irytacja</trd>
  </trdgrp>
</drv>

<drv mrk="ekscit.0igxema">
  <kap><tld/>i&gcirc;ema</kap>
  <snc mrk="ekscit.0igxema.PSI">
    <dif>
      Kiu facile <tld/>i&gcirc;as, vigli&gcirc;as, pasias;
      <ref tip="vid" cel="nerv.0ema.PSI">nervema</ref>:
      <ekz>
        amba&ubreve; virinoj videble posedis karakterojn vivajn kaj <tld/>i&gcirc;emajn
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi estas <tld/>i&gcirc;ema viro kiam mi vere koleri&gcirc;as
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">soupe au lait<klr> (adj.)</klr></trd>
</drv>

