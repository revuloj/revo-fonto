<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: granol.xml,v 1.26 2025/09/04 20:31:40 revo Exp $">
<kap>
  <rad>granol</rad>/o <fnt><bib>PVEF</bib></fnt>,
  <var><kap><rad var="u">granul</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>
<drv mrk="granol.0o">
  <kap><tld/>o, <var><kap><tld var="u"/>o</kap></var></kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Malgranda <ref tip="super" cel="pilol.0o">pilolo</ref>, kiu ne pezas pli ol kvin centigramojn:
      <ekz>&scirc;i donis al la infano du <tld/>ojn kontra&ubreve; la doloro.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bolus.0o">boluso</ref>
    </refgrp>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru (malgranda) <ke>pilolo</ke>, <ke>grajneto</ke>.
  </rim>

  <trd lng="be">&c_g;&c_r;&c_a;&c_n;&c_u;&c_l;&c_a; <klr>(&c_l;&c_je;&c_k;&c_a;&c_w;)</klr></trd>
  <trd lng="ca">gr&agrave;nul</trd>
  <trdgrp lng="cs">
    <trd>granule mal&aacute;</trd>,
    <trd>pilulka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Granulum</trd>,
    <trd>K&ouml;rnchen</trd>,
    <trd>K&uuml;gelchen</trd>
  </trdgrp>
  <trd lng="fr">granule</trd>
  <trdgrp lng="hu">
    <trd>labdacs</trd>,
    <trd>pirula</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#23567;&#31890;  <pr>&#12371;&#12388;&#12406;</pr></trd>,
    <trd>&#32048;&#31890;  <pr>&#12373;&#12356;&#12426;&#12421;&#12358;</pr></trd>,
    <trd>&#38982;&#31890;  <pr>&#12363;&#12426;&#12421;&#12358;</pr></trd>,
    <trd>&#38982;&#31890;&#21092;  <pr>&#12363;&#12426;&#12421;&#12358;&#12374;&#12356;</pr></trd>,
    <trd>&#32048;&#31890;&#20024;&#34220;  <pr>&#12411;&#12381;&#12388;&#12406;&#12364;&#12435;&#12420;&#12367;</pr></trd>,
    <trd>&#31890;&#29366;&#26001;  <pr>&#12426;&#12421;&#12358;&#12376;&#12423;&#12358;&#12399;&#12435;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>granule</trd>,
    <trd>(homeopatische) <ind>korrel</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>granulka</trd>,
    <trd>ziarenko</trd>
  </trdgrp>
  <trd lng="pt">gr&acirc;nulo</trd>
  <trd lng="ro">granul&abreve;</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_n;&c_u;&c_l;&c_a;</trd>
  <trd lng="sk">tabletka</trd>
  <trdgrp lng="uk">
    <trd>&c_gu;&c_r;&c_a;&c_n;&c_u;&c_l;&c_a;</trd>,
    <trd>&c_r;&c_ib;&c_d;&c_sh;&c_je;&nbsp;&c_m;&c_a;&c_l;&c_a;&nbsp;&c_p;&c_ib;&c_gu;&c_u;&c_l;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#38982;&#31890; <pr>k&emacron;l&igrave;</pr></trd>
</drv>
</art>
