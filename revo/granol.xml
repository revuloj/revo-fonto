<vortaro>
<art mrk="$Id$">
<kap>
  <rad>granol</rad>/o
</kap>
<drv mrk="granol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Malgranda pilolo, kiu ne pezas pli ol kvin centigramojn.
    </dif>
    <refgrp tip="vid">
      <ref cel="bolus.0o">boluso</ref>,
      <ref cel="pilol.0o">pilolo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Granulum</trd>,
    <trd>K&ouml;rnchen</trd>
  </trdgrp>
  <trd lng="fr">granule</trd>
  <trd lng="nl">(homeopatische) <ind>korrel</ind></trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_n;&c_u;&c_l;&c_a;</trd>
</drv>
</art>
