<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: granol.xml,v 1.15 2022/03/19 19:18:24 revo Exp $">
<kap>
  <rad>granol</rad>/o
</kap>
<drv mrk="granol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Malgranda pilolo, kiu ne pezas pli ol kvin centigramojn.
    </dif>
    <refgrp tip="vid">
      <ref cel="bolus.0o">boluso</ref>,
      <ref cel="pilol.0o">pilolo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_n;&c_u;&c_l;&c_a; <klr>(&c_l;&c_je;&c_k;&c_a;&c_w;)</klr></trd>
  <trd lng="ca">gr&agrave;nul</trd>
  <trdgrp lng="cs">
    <trd>granule mal&#xE1;</trd>,
    <trd>pilulka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Granulum</trd>,
    <trd>K&ouml;rnchen</trd>
  </trdgrp>
  <trd lng="fr">granule</trd>
  <trdgrp lng="hu">
    <trd>labdacs</trd>,
    <trd>pirula</trd>
  </trdgrp>
  <trd lng="nl">(homeopatische) <ind>korrel</ind></trd>
  <trd lng="pt">gr&acirc;nulo</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_n;&c_u;&c_l;&c_a;</trd>
  <trd lng="sk">tabletka</trd>
  <trdgrp lng="uk">
    <trd>&#x491;&#x440;&#x430;&#x43D;&#x443;&#x43B;&#x430;</trd>,
    <trd>&#x440;&#x456;&#x434;&#x448;&#x435;&#xA0;&#x43C;&#x430;&#x43B;&#x430;&#xA0;&#x43F;&#x456;&#x491;&#x443;&#x43B;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>
</art>
