<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>hist</rad>/o
    </kap>

    <drv mrk="hist.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">ANA</uzo>
      <snc>
	<dif>
      	  Sistemo da samfunkciaj kaj samdevenaj
	  <ref tip="dif" cel="cxel.0o.BIO">&ccirc;eloj</ref>, kune kun la
	  inter&ccirc;elaj substancoj kaj la strukturoj produktitaj de la
	  &ccirc;eloj:
	  <ekz>
            <ind>mola <tld/>o</ind>;
            <trd lng="en">soft tissue</trd>
            <trd lng="ru">&c_m;&c_ja;&c_g;&c_k;&c_a;&c_ja; &c_t;&c_k;&c_a;&c_n;&c_mol;</trd>
          </ekz>
	  <ekz>
            <ind>kuniga <tld/>o</ind>;
            <trd lng="en">connective tissue</trd>
            <trd lng="ru">&c_s;&c_o;&c_je;&c_d;&c_i;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_t;&c_k;&c_a;&c_n;&c_mol;</trd>
          </ekz>
	  <ekz>
            <ind>osto<tld/>o</ind>.
            <trd lng="en">bone tissue</trd>
            <trd lng="ru">&c_k;&c_o;&c_s;&c_t;&c_n;&c_a;&c_ja; &c_t;&c_k;&c_a;&c_n;&c_mol;</trd>
          </ekz>
        </dif>
      </snc>
      <trd lng="de">(organisches) <ind>Gewebe</ind></trd>
      <trd lng="el">&jota;&sigma;&tau;&omikron_ton;&sigma_fina;</trd>
      <trd lng="en">tissue</trd>
      <trd lng="es">tejido <klr tip="ind"> (org&aacute;nico)</klr></trd>
      <trd lng="fr">tissu<klr tip="ind"> (cellulaire)</klr></trd>
      <trd lng="nl">weefsel<klr tip="ind"> (organisch)</klr></trd>
      <trd lng="pl">tkanka</trd>
      <trd lng="pt">tecido<klr tip="ind"> (celular)</klr></trd>
      <trd lng="ru">&c_t;&c_k;&c_a;&c_n;&c_mol;<klr tip="ind"> (&c_b;&c_i;&c_o;&c_l;.)</klr></trd>
    </drv>
  </art>
