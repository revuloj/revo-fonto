<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>a&ubreve;topsi</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="auxtopsi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <ref tip="dif" cel="nekrop1.0o">nekropsio</ref>
    <ekz>
      mi petis la distriktan kuraciston, ke li permesu al mi fari la
      <tld/>on
      <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 27a, p. 194a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_r;&c_o;&c_s;&c_p;&c_a;&c_l;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>Nekropsie</trd>,
    <trd>Obduktion</trd>
  </trdgrp>
  <trd lng="el">&ny;&epsilon;&kappa;&rho;&omikron;&psi;&jota_ton;&alfa;</trd>
  <trdgrp lng="en">
    <trd>autopsy</trd>,
    <trd>necropsy</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>autopsia</trd>,
    <trd>necropsia</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>autopsie</trd>
  </trdgrp>
  <trd lng="hu">boncol&aacute;s</trd>
  <trd lng="it">autopsia</trd>
  <trdgrp lng="nl">
    <trd>sectie</trd>,
    <trd>dissectie</trd>,
    <trd>necropsie</trd>,
    <trd>lijkschouwing</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>aut&oacute;psia</trd>,
    <trd>necropsia</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_s;&c_k;&c_r;&c_y;&c_t;&c_i;&c_je; <klr>(&c_p;&c_o;&c_s;&c_m;&c_je;&c_r;&c_t;&c_n;&c_o;&c_je;)</klr></trd>
</drv>
</art>

