<art mrk="$Id: auxtopsi.xml,v 1.9 2023/10/28 08:33:10 revo Exp $">
<kap>
  <rad>a&ubreve;topsi</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="auxtopsi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc mrk="auxtopsi.0o.nekropsio">
    <dif>
      <ref tip="dif" cel="nekrop1.0o">Nekropsio</ref>:
      <ekz>
        mi petis la distriktan kuraciston, ke li permesu al mi fari la
        <tld/>on
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 27a, p. 194a</lok></fnt>;
      </ekz>
      <ekz>
        <nom>John McCrae</nom> rapide avancis kiel kuracisto, laboris pri <tld/>oj, pri patologio,
        pri konta&gcirc;aj malsanoj
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="&Monato;/2015/011504.php">Tre bonvena verko, sed ...</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x89E3;&#x5256; <pr>ji&#x11B;p&#x1D2;u</pr></trd>,
    <trd>&#x5C38;&#x68C0; <pr>sh&#x12B;ji&#x1CE;n</pr></trd>,
    <trd>&#x5C38;&#x6AA2; <pr>sh&#x12B;ji&#x1CE;n</pr></trd>,
    <trd>&#x5C38;&#x4F53;&#x5256;&#x68C0; <pr>sh&#x12B;t&#x1D0;p&#x14D;uji&#x1CE;n</pr></trd>,
    <trd>&#x5C38;&#x9AD4;&#x5256;&#x6AA2; <pr>sh&#x12B;t&#x1D0;p&#x14D;uji&#x1CE;n</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_r;&c_o;&c_s;&c_p;&c_a;&c_l;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_a;&c_u;&c_t;&c_o;&c_p;&c_s;&c_i;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Nekropsie</trd>,
    <trd>Obduktion</trd>
  </trdgrp>
  <trd lng="el">&ny;&epsilon;&kappa;&rho;&omikron;&psi;&jota_ton;&alfa;</trd>
  <trdgrp lng="en">
    <trd>autopsy</trd>,
    <trd>necropsy</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>autopsia</trd>,
    <trd>necropsia</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>autopsie</trd>
  </trdgrp>
  <trd lng="he">&#x5E0;&#x5EA;&#x5D9;&#x5D7;&#x5D4;</trd>
  <trd lng="hu">boncol&aacute;s</trd>
  <trd lng="it">autopsia</trd>
  <trdgrp lng="nl">
    <trd>sectie</trd>,
    <trd>dissectie</trd>,
    <trd>necropsie</trd>,
    <trd>lijkschouwing</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>aut&oacute;psia</trd>,
    <trd>necropsia</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_s;&c_k;&c_r;&c_y;&c_t;&c_i;&c_je; <klr>(&c_p;&c_o;&c_s;&c_m;&c_je;&c_r;&c_t;&c_n;&c_o;&c_je;)</klr></trd>
  <trd lng="uk">&#x430;&#x432;&#x442;&#x43E;&#x43F;&#x441;&#x456;&#x44F;</trd>
</drv>
