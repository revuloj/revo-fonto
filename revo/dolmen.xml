<art mrk="$Id: dolmen.xml,v 1.14 2023/08/31 10:44:01 revo Exp $">
<kap>
   <rad>dolmen</rad>/o
</kap>
<drv mrk="dolmen.0o">
  <kap><tld/>o</kap>
  <snc mrk="dolmen.0o.ARKE">
    <uzo tip="fak">ARKE</uzo>
    <dif>
      Megalita <ref tip="super" cel="monume.0o.ARKI">monumento</ref> el
      pluraj &scirc;tonegoj kiuj ofte prezenti&gcirc;as en tabla formo:
      <ekz>
        li prifosadas <tld/>on &ccirc;e Longa Monteto, kaj trovis 
        prahoman kranion, kiu plen&gcirc;ojigas lin
        <fnt>
          <aut>Conan Doyle, Arthur, trad.William Auld</aut>,
          <vrk><url ref="&LOdE;Chashund.htm">
          La &ccirc;ashundo de la Baskerviloj</url></vrk>,
          <lok>&ccirc;apitro 8a</lok>
        </fnt>;
      </ekz>
      <ekz>
        vizito de proksima <tld/>o de la Kovrita &Scirc;tono
        <fnt>
          <vrk><url ref="https://gresillon.org/spip.php?article183">Esperanto en Artoj
          (2018-jul-21/27)</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="super">
       <ref cel="megali.0o">megalito</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_l;&c_mol;&c_m;&c_e;&c_n;</trd>
  <trd lng="bg">&c_d;&c_o;&c_l;&c_m;&c_je;&c_n;</trd>
  <trd lng="ca">dolmen</trd>
  <trd lng="cs">dolmen</trd>
  <trdgrp lng="de">
    <trd>H&uuml;nengrab</trd>,
    <trd>Dolmen</trd>
  </trdgrp>
  <trd lng="fr">dolmen</trd>
  <trdgrp lng="he">
    <trd>&#x5D3;&#x5D5;&#x5DC;&#x5DE;&#x5DF;</trd>,
    <trd>&#x5DE;&#x5E6;&#x5D1;&#x5EA; &#x5D0;&#x5D1;&#x5DF;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30C9;&#x30EB;&#x30E1;&#x30F3;</trd>,
    <trd>&#x652F;&#x77F3;&#x5893;  [<ind>&#x3057;&#x305B;&#x304D;&#x307C;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
     <trd>hunebed</trd>,
     <trd>dolmen</trd>
   </trdgrp>
  <trd lng="pl">dolmen</trd>
  <trdgrp lng="sk">
    <trd>dolmen</trd>,
    <trd>kamenn&yacute; n&aacute;hrobok</trd>
  </trdgrp>
  <trd lng="uk">&c_d;&#x43E;&#x43B;&#x44C;&#x43C;&c_je;&#x43D;</trd>
</drv>
</art>

