<art mrk="$Id: dolmen.xml,v 1.16 2023/11/21 21:23:26 revo Exp $">
<kap>
   <rad>dolmen</rad>/o
   <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="dolmen.0o">
  <kap><tld/>o</kap>
  <snc mrk="dolmen.0o.ARKE">
    <uzo tip="fak">ARKE</uzo>
    <dif>
      Megalita <ref tip="super" cel="monume.0o.ARKI">monumento</ref> el
      pluraj &scirc;tonegoj kiuj ofte prezenti&gcirc;as en tabla formo:
      <ekz>
        li prifosadas <tld/>on &ccirc;e Longa Monteto, kaj trovis
        prahoman kranion, kiu plen&gcirc;ojigas lin
        <fnt>
          <aut>Conan Doyle, Arthur, trad.William Auld</aut>,
          <vrk><url ref="&LOdE;Chashund.htm">
          La &ccirc;ashundo de la Baskerviloj</url></vrk>,
          <lok>&ccirc;apitro 8a</lok>
        </fnt>;
      </ekz>
      <ekz>
        vizito de proksima <tld/>o de la Kovrita &Scirc;tono
        <fnt>
          <vrk><url ref="https://gresillon.org/spip.php?article183">Esperanto en Artoj
          (2018-jul-21/27)</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="super">
       <ref cel="megali.0o">megalito</ref>
    </refgrp>
    <ref tip="vid" cel="menhir.0o">menhiro</ref>
    <bld
      lok="&WCU;/thumb/2/2a/Pontign&eacute;_-_Dolmen_3.jpg/640px-Pontign&eacute;_-_Dolmen_3.jpg"
      prm="CC BY-SA 3.0"
      ><tld/>o &ccirc;e Greziljono
        <fnt>
          <aut>Kormin, propra verko</aut>
          <lok><url ref="&WCW;/File:Pontign%C3%A9_-_Dolmen_3.jpg"
              >https://commons.wikimedia.org/wiki/File:Pontign&eacute;_-_Dolmen_3.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_l;&c_mol;&c_m;&c_e;&c_n;</trd>
  <trd lng="bg">&c_d;&c_o;&c_l;&c_m;&c_je;&c_n;</trd>
  <trd lng="ca">dolmen</trd>
  <trd lng="cs">dolmen</trd>
  <trdgrp lng="de">
    <trd>H&uuml;nengrab</trd>,
    <trd>Dolmen</trd>
  </trdgrp>
  <trd lng="fr">dolmen</trd>
  <trdgrp lng="he">
    <trd>&dalet;&vav;&lamed;&mem;&fnun;</trd>,
    <trd>&mem;&tsadi;&bet;&tav; &alef;&bet;&fnun;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12489;&#12523;&#12513;&#12531;</trd>,
    <trd>&#25903;&#30707;&#22675;  <pr>&#12375;&#12379;&#12365;&#12412;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
     <trd>hunebed</trd>,
     <trd>dolmen</trd>
   </trdgrp>
  <trd lng="pl">dolmen</trd>
  <trdgrp lng="sk">
    <trd>dolmen</trd>,
    <trd>kamenn&yacute; n&aacute;hrobok</trd>
  </trdgrp>
  <trd lng="uk">&c_d;&c_o;&c_l;&c_mol;&c_m;&c_je;&c_n;</trd>
</drv>
</art>

