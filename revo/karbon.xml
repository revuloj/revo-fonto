<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: karbon.xml,v 1.32 2023/12/11 22:30:41 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>karbon</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="karbon.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">C</mlg>
  <snc mrk="karbon.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="6">elemento</ref>
      kun atomnumero 6, atompezo 12,011115;
      el kiu precipe konsistas
      <ref tip="sub" cel="karb.0o">karbo</ref>,
      <ref tip="sub" cel="grafit.0o">grafito</ref>,
      <ref tip="sub" cel="diaman.0o">diamanto</ref>,
      kaj kiu trovi&gcirc;as en &ccirc;iu konata viva&jcirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_g;&c_l;&c_ja;&c_r;&c_o;&c_d;</trd>
  <trd lng="bg">&c_v;&c_malmol;&c_g;&c_l;&c_je;&c_r;&c_o;&c_d;</trd>
  <trd lng="br">karbon</trd>
  <trd lng="ca">carboni</trd>
  <trdgrp lng="cs">
    <trd>carboneum</trd>,
    <trd>uhl&iacute;k</trd>
  </trdgrp>
  <trd lng="de">Kohlenstoff</trd>
  <trd lng="en">carbon</trd>
  <trd lng="es">carbono</trd>
  <trd lng="fr">carbone</trd>
  <trdgrp lng="hu">
    <trd>elemi <ind>sz&eacute;n</ind></trd>,
    <trd>karbon</trd>
  </trdgrp>
  <trd lng="id">karbon</trd>
  <trd lng="it">carbonio</trd>
  <trd lng="ja">&#x70AD;&#x7D20;  <pr>&#x305F;&#12435;&#x305D;</pr></trd>
  <trd lng="la">Carboneum</trd>
  <trd lng="nl">koolstof</trd>
  <trd lng="pl">w&eogonek;giel</trd>
  <trd lng="pt">carbono</trd>
  <trd lng="ru">&c_u;&c_g;&c_l;&c_je;&c_r;&c_o;&c_d;</trd>
  <trd lng="sk">uhl&iacute;k</trd>
  <trd lng="sv">kol</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_a;&c_r;&c_b;&#x43E;&#x43D; (&c_h;&c_ib;&#x43C;&c_ib;&c_ch;&#x43D;&c_i;&c_j; &c_je;&#x43B;&c_je;&#x43C;&c_je;&#x43D;&c_t;)</trd>,
    <trd>&c_v;&c_u;&c_g;&#x43B;&c_je;&c_c;&#x44C; (&#x43F;&c_r;&#x43E;&c_s;&c_t;&c_a; &c_r;&c_je;&c_ch;&#x43E;&c_v;&c_i;&#x43D;&c_a;)</trd>
  </trdgrp>
  <trd lng="zh">&#x78B3; <pr>t&#xE0;n</pr></trd>
</drv>

<drv mrk="karbon.radi0o">
  <kap>radikarbono,
  <var><kap>radio<tld/>o</kap></var>,
  <var><kap><tld/>o 14</kap></var></kap>
  <snc mrk="karbon.radi0o.KEM">
    <uzo tip="fak">ARKE</uzo>
    <uzo tip="fak">KAL</uzo>
    <uzo tip="fak">KEM</uzo>
    <dif>
      Radiaktiva izotopo de <tld/>o kun atompezo 14,
      per kies mezuro en organika&jcirc;oj oni taksas ilian antikvecon,
      helpe al <ref tip="vid" cel="kronol.0o.SCI">kronologio</ref>;
      simbole <frm><sup>14</sup>C</frm>:
      <ekz>
        uzo de radio<tld/>o por la determino de la a&gcirc;o de
         organika&jcirc;oj
        <fnt>
        <aut>H. Sirk</aut>,
        <vrk>La uzo de radiokarbono...</vrk>,
        <lok>Scienca Revuo, 1959, numero 2-3a</lok>
        </fnt>;
      </ekz>
      <ekz>
        estis grava la determino de la relativa kvanto da <tld/>o 14,
        per kiu eblas la datado de historiaj kaj anta&ubreve;historiaj trova&jcirc;oj
        <fnt>
          <aut>E. Pauli</aut>,
          <vrk><url ref="http://www.cfh.ufsc.br/~simpozio/Esperanto/metodo-pensado/4951-031.htm">
          Metodoj de Pensado</url></vrk>,
          <lok>1977</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_d;&c_y;&c_je;&c_a;&c_k;&c_t;&c_y;&c_w;&c_n;&c_y; &c_v;&c_u;&c_g;&c_l;&c_ja;&c_r;&c_o;&c_d;</trd>,
    <trd>&c_v;&c_u;&c_g;&c_l;&c_ja;&c_r;&c_o;&c_d;-14</trd>
  </trdgrp>
  <trd lng="ca">carboni 14</trd>
  <trdgrp lng="fr">
    <trd>carbone 14</trd>,
    <trd>radiocarbone</trd>
  </trdgrp>
  <trd lng="pl">w&eogonek;giel-14</trd>
</drv>
</art>
