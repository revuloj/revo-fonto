<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
   <art mrk="$Id$">
     <kap>
       <ofc>4</ofc>
       <rad>karbon</rad>/o
     </kap>
     <drv mrk="karbon.0o">
       <kap><tld/>o</kap>
       <uzo tip="fak">KEM</uzo>
       <snc>
         <dif>
           &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref>
           kun atomnumero 6, atompezo 12,011115, simbolo <frm>C</frm>.
           Tiu elemento, el kiu precipe konsistas karbo, grafito,
           diamanto, kaj kiu trovi&gcirc;as en &ccirc;iu vivanta 
esta&jcirc;o:
           vegeta&jcirc;o a&ubreve; animalo.
         </dif>
       </snc>
       <trd lng="be">&c_v;&c_u;&c_g;&c_l;&c_ja;&c_r;&c_o;&c_d;</trd>   
       <trd lng="br">karbon</trd>
       <trd lng="ca">carboni</trd>
       <trd lng="de">Kohlenstoff</trd>
       <trd lng="en">carbon</trd>
       <trd lng="es">carbono</trd>
       <trd lng="fr">carbone</trd>
       <trdgrp lng="hu">
         <trd>elemi <ind>sz&eacute;n</ind></trd>,
         <trd>karbon</trd>
       </trdgrp>
       <trd lng="it">carbonio</trd>
       <trd lng="nl">koolstof</trd>
       <trd lng="pt">carbono</trd>
       <trd lng="ru">&c_u;&c_g;&c_l;&c_je;&c_r;&c_o;&c_d;</trd>
     </drv>
   </art>
