<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kand.xml,v 1.25 2025/03/31 16:20:08 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kand</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="kand.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kand.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Purigita, kristaligita kaj preska&ubreve; diafana
      <ref tip="super" cel="suker.0o">sukero</ref>:
      <ekz>
        <klr>[la]</klr> &ccirc;ambristino <klr>[&#8230;]</klr> regalis Iva&ccirc;jon per <tld/>oj kaj
        simple por amuzo instruis lin legi
        <fnt>
          <lok><url ref="&Tekstaro;?nomo=ondo-de-esperanto">&Tekstaro;?nomo=ondo-de-esperanto</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;in profunde okupis negoco pri <tld/>aj bestetoj
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="karame.0o">karamelo</ref>
    </refgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: (en la senco de <ke>bombono</ke>) prefere diru <ke>bombono</ke>, <ke>dol&#265;a&#309;o</ke>.
  </rim>

  <trd lng="be">&c_l;&c_ja;&c_d;&c_z;&c_ja;&c_sh;</trd>
  <trdgrp lng="cs">
    <trd>kandys</trd>,
    <trd>zast. cukrkandl</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Kandis</trd>,
     <trd>Kandiszucker</trd>,
     <trd><ofc>*</ofc>Kandelzucker</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd><ofc>*</ofc>sugar candy</trd>,
    <trd>crystallized sugar</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>caramelo</trd>,
    <trd>az&uacute;car caramelizado</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd><ofc>*</ofc>candi <klr>(sucre)</klr></trd>,
    <trd>sucre candi</trd>
  </trdgrp>
  <trdgrp lng="hu">
   <trd>kandiscukor</trd>,
   <trd>jegescukor</trd>
  </trdgrp>
  <trd lng="ja">&#27703;&#30722;&#31958;  <pr>&#12371;&#12362;&#12426;&#12374;&#12392;&#12358;</pr></trd>
  <trd lng="nl">kandij</trd>
  <trdgrp lng="pl">
    <trd><ofc>*</ofc>cukier lodowaty</trd>,
    <trd>kandyz</trd>
  </trdgrp>
  <trd lng="ro">bomboane</trd>
  <trd lng="ru"><ofc>*</ofc>&c_l;&c_je;&c_d;&c_je;&c_n;&c_je;&c_c;</trd>
  <trd lng="sk">kandys</trd>
  <trd lng="uk">&c_l;&c_mol;&c_o;&c_d;&c_ja;&c_n;&c_i;&c_k;</trd>
  <trd lng="zh">&#20912;&#31958; <pr>b&#299;ngt&#225;ng</pr></trd>
</drv>

<drv mrk="kand.0i">
  <kap>
    <tld/>i
  </kap>
  <snc mrk="kand.0i.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      &Ccirc;irka&ubreve;kovri per tavolo de <tld/>o:
      <ekz>
        <tld/>ita zingibro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4622">Zingibro</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_k;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol; &c_c;&c_u;&c_k;&c_r;&c_o;&c_v;&c_a;&c_j; <ind>&c_g;&c_l;&c_a;&c_z;&c_u;&c_r;&c_a;&c_j;</ind></trd>
  <trd lng="de">kandieren</trd>
  <trd lng="en">candy</trd>
  <trdgrp lng="zh">
    <trd>&#20351;&#25104;&#20026;&#31958;&#32467;&#26230;
    <pr>sh&#464;ch&#233;ngw&#233;it&#225;ngji&#233;j&#299;ng</pr></trd>,
    <trd>&#20570;&#31958;&#26524; <pr>zu&#242;t&#225;nggu&#466;</pr></trd>,
    <trd>&#22312;&#31957;&#28857;&#19978;&#27922;&#31958;
    <pr>z&#224;ig&amacron;odi&#462;nsh&#224;ngs&#462;t&#225;ng</pr></trd>
  </trdgrp>
</drv>

</art>
