<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kand</rad>/o
</kap>
<drv mrk="kand.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kand.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Purigita, kristaligita kaj preska&ubreve; diafana sukero:
      <ekz>
        &scirc;in profunde okupis negoco pri <tld/>aj bestetoj
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>  
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_d;&c_z;&c_ja;&c_sh;</trd>
  <trdgrp lng="de">
     <trd>Kandis</trd>,
     <trd>Kandiszucker</trd>,
     <trd>Kandelzucker</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>candi</trd>,
    <trd>sucre candi</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>kandiscukor</trd>,
     <trd>jegescukor</trd>
  </trdgrp>
  <trd lng="nl">kandij</trd>
  <trd lng="ru">&c_l;&c_je;&c_d;&c_je;&c_n;&c_je;&c_c;</trd>
</drv>
</art>
