     <trd>Kandiszucker</trd>,
     <trd>Kandelzucker</trd>
  </trdgrp>
  <trd lng="nl">kandij</trd>
  <trd lng="ru">&c_l;&c_je;&c_d;&c_je;&c_n;&c_je;&c_c;</trd>
</drv>
</art>
