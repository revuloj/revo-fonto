<art mrk="$Id$">
<kap>
  <rad>naftol</rad>/o
</kap>
<drv mrk="naftol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      C<sub>10</sub>H<sub>7</sub>O<sub>4</sub>. Fenolo ekstraktata el
      naftalino, kaj uzata kiel intesta desinfekta&jcirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_f;&c_t;&c_o;&c_l;</trd>
  <trd lng="de">Naphtol</trd>
  <trd lng="en">naphthol</trd>
  <trd lng="hu">naftol</trd>
  <trd lng="nl">naftol</trd>
  <trd lng="ru">&c_n;&c_a;&c_f;&c_t;&c_o;&c_l;</trd>
</drv>
</art>
