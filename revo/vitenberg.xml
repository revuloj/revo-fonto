<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: vitenberg.xml,v 1.3 2022/05/06 12:17:22 afido Exp $">
    <kap>
      <rad>Vitenberg</rad>/o, 
      <var><kap><rad var="tt">Vittenberg</rad>/o  <fnt><bib>Hamlet</bib></fnt></kap></var>
    </kap>

<drv mrk="vitenberg.0o">
  <kap><tld/>o, <var><kap><tld var="tt"/></kap></var></kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="vitenberg.0o.urbo">
    <dif>
      <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">Urbo</ref> 
      &ccirc;e la rivero <ref cel="elb.E0o.rivero">Elbo</ref>, &ccirc;. 90&#8239;km sudokcidente de 
      <ref cel="berlin.0o">Berlino</ref>, vivurbo de <ref tip="prt" cel="luter.0o">Lutero</ref>
      kaj centro de la <ref cel="reform.0o">Reformacio</ref>:
      <ekz>
        por kio vi forlasis <tld var="tt"/>on
        <fnt>
          <bib>Hamlet</bib>
        </fnt>?
      </ekz>
      <ekz>
        la Universitato de <tld/>o
        <fnt>
          <vrk><url ref="&Viki;/Universitato_de_Vitenbergo">Universitato de
          Wittenberg</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Vitenbergo situas <klr>[&#8230;]</klr> &ccirc;irka&ubreve; mezvoje inter Berlino kaj Lepsiko
        <fnt>
          <vrk><url ref="https://eo.wikivoyage.org/wiki/Vitenbergo">Vitenbergo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_V;&c_ib;&c_t;&c_e;&c_n;&c_b;&c_e;&c_r;&c_g;</trd>
    <trd lng="de">Wittenberg</trd>
    <trd lng="en">Wittenberg</trd>
    <trd lng="fr">Wittemberg</trd>
    <trd lng="ru">&c_V;&c_i;&c_t;&c_t;&c_je;&c_n;&c_b;&c_je;&c_r;&c_g;</trd>
  </snc>
</drv>

  </art>

