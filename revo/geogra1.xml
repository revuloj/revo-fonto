<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: geogra1.xml,v 1.34 2023/08/31 14:29:52 revo Exp $">
<kap>
  <ofc>*</ofc><ofc>1</ofc><ofc>7</ofc>
  <rad>geografi</rad>/o
  <fnt><bib>F</bib>, <lok>Ekzercaro, &para; 3</lok></fnt>
</kap>
<drv mrk="geogra1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="geogra1.0o.SCI">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="lst" cel="scienc.0o.fako" lst="voko:sciencoj">Scienco</ref>,
      kiu pritraktas la aspekton de la diversaj regionoj de la
      <ref tip="prt" cel="ter.0o.planedo">tero</ref>:
      <ekz>
        nomoj propraj kaj <ind><tld/>a</ind>j ricevis en la vortaro
        apartan
        lokon;
        <trd lng="fr">g&eacute;ographique</trd>
        <trd lng="nl">geografisch</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">ARK</uzo>
        <ind><tld/>a karto</ind>
        <fnt><bib>Fab4</bib>, <lok>Malbona princo</lok></fnt>
        <klr>(nun prefere: <tld/>a <ref cel="map.0o">mapo</ref>)</klr>.
        <trd lng="ca">mapa</trd>
        <trd lng="fr">carte de g&eacute;ographie</trd>
        <trd lng="nl">landkaart</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_a;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_g;&c_je;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
  <trd lng="ca">geografia</trd>
  <trdgrp lng="cs">
    <trd>geografie</trd>,
    <trd>zem&ecaron;pis</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Geographie</trd>,
    <trd>Erdkunde</trd>
  </trdgrp>
  <trd lng="en">geography</trd>
  <trd lng="es">geograf&iacute;a</trd>
  <trd lng="fa">&a_j;&a_gh;&a_r;&a_A;&a_f;&f_y;&a_A;</trd>
  <trd lng="fr">g&eacute;ographie</trd>
  <trd lng="id">geografi</trd>
  <trdgrp lng="ja">
    <trd>&#x5730;&#x7406;&#x5B66;  [<ind>&#x3061;&#x308A;&#x304C;&#x304F;</ind>]</trd>,
    <trd>&#x5730;&#x7406;  [<ind>&#x3061;&#x308A;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>aardrijkskunde</trd>,
    <trd>geografie</trd>
  </trdgrp>
  <trd lng="pl">geografia</trd>
  <trd lng="pt">geografia</trd>
  <trd lng="ru">&c_g;&c_je;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
  <trd lng="sk">zemepis</trd>
  <trd lng="sw">jiografia</trd>
  <trd lng="tr">jeografi</trd>
  <trd lng="uk">&#x433;&#x435;&#x43E;&#x433;&#x440;&#x430;&#x444;&#x456;&#x44F;</trd>
</drv>

<drv mrk="geogra1.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="geogra1.0isto.SCI">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Fakulo pri <tld/>o.
    </dif>
    <ref tip="sin" cel="geogra.0o">geografo.</ref>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
  <trd lng="bg">&c_g;&c_je;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
  <trd lng="ca">ge&ograve;graf</trd>
  <trdgrp lng="cs">
    <trd>geograf</trd>,
    <trd>zpracovatel map</trd>
  </trdgrp>
  <trd lng="de">Geograph</trd>
  <trd lng="en">geographer</trd>
  <trd lng="es">ge&oacute;grafo</trd>
  <trd lng="fa">&a_j;&a_gh;&a_r;&a_A;&a_f;&f_y;&a_d;&a_A;&a_n;</trd>
  <trd lng="fr">g&eacute;ographe</trd>
  <trd lng="id">geograf</trd>
  <trdgrp lng="nl">
    <trd>geograaf</trd>,
    <trd>aardrijkskundige</trd>
  </trdgrp>
  <trd lng="pl">geograf</trd>
  <trd lng="pt">ge&oacute;grafo</trd>
  <trd lng="ru">&c_g;&c_je;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
  <trdgrp lng="sk">
    <trd>geograf</trd>,
    <trd>zemepisec</trd>
  </trdgrp>
</drv>

<drv mrk="geogra1.fizika0o">
  <kap>fizika <tld/>o, <var><kap>natur<tld/>o</kap></var></kap>
  <snc mrk="geogra1.fizika0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Parto de <ref tip="malprt" cel="geogra1.0o.SCI">geografio</ref>
      studanta la naturan konsiston de landoj, iliajn reliefojn,
      grundojn, akvejojn, naturmediojn...:
      <ekz>
        d<klr>oktor</klr>ino Ieva Svarcaite en 2006 <klr>...</klr>
        ricevis internacian sciencan  titolon Doktoro de Natursciencoj
        pro <klr>...</klr> terminaro de fizika <tld/>o
        <fnt>
          <aut>P. Jegorovas</aut>,
          <vrk>Nova&jcirc;oj</vrk>,
          <lok><url ref="http://www.esperanto.lt/">
          Litova Esperanto-Asocio</url>, 2007-03-17</lok>
        </fnt>;
      </ekz>
      <ekz>
        Glazunov <klr>...</klr> studis natur<tld/>on kaj multe
        voja&gcirc;is
        en Sovetunio kun la poeto Konstantin Gusev, kies verkojn li
        tradukis
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.esperanto.net/literaturo/">
          Originala literaturo Esperanta</url></vrk>,
          <lok>artikolo &leftquot; Vladimir Aleksandrovi&ccirc;
Glazunov&rightquot;, [2007-03]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_z;&c_y;&c_ch;&c_n;&c_a;&c_ja;
  <ind>&c_g;&c_je;&c_a;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</ind></trd>
  <trd lng="ca">fisiografia</trd>
  <trdgrp lng="en">
    <trd>physical geography</trd>,
    <trd>physiography</trd>
  </trdgrp>
  <trd lng="fa">&a_j;&a_gh;&a_r;&a_A;&a_f;&f_y;&a_A;&f_y; &a_T;&a_b;&f_y;&a_ayn;&f_y;</trd>
  <trd lng="fr">g&eacute;ographie physique</trd>
  <trd lng="id"><ind>geografi</ind> fisik</trd>
  <trd lng="pl">geografia fizyczna</trd>
  <trdgrp lng="pt">
    <trd>geografia f&iacute;sica</trd>,
    <trd>fisiografia</trd>
  </trdgrp>
</drv>
</art>
