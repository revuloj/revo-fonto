<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>geografi</rad>/o
<fnt><bib>F</bib>, <lok>Ekzercaro, &para; 3</lok></fnt>
</kap>
<drv mrk="geogra1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SCI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="scienc.0o.fako">Scienco</ref>,
      kiu pritraktas la aspekton de la diversaj regionoj de la
      <ref tip="prt" cel="ter.0o.planedo">tero</ref>:
      <ekz>
        nomoj propraj kaj <tld/>aj ricevis en la vortaro apartan lokon;
      </ekz>
      <ekz>
        <tld/>a karto
        <fnt><bib>Fab4</bib>, <lok>Malbona princo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Geographie</trd>
</drv>
</art>
