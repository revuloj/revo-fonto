<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>geografi</rad>/o
  <fnt><bib>F</bib>, <lok>Ekzercaro, &para; 3</lok></fnt>
</kap>
<drv mrk="geogra1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SCI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="scienc.0o.fako">Scienco</ref>,
      kiu pritraktas la aspekton de la diversaj regionoj de la
      <ref tip="prt" cel="ter.0o.planedo">tero</ref>:
      <ekz>
        nomoj propraj kaj <ind><tld/>a</ind>j ricevis en la vortaro apartan 
        lokon;
        <trd lng="fr">g&eacute;ographique</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">ARK</uzo>
        <ind><tld/>a karto</ind>
        <fnt><bib>Fab4</bib>, <lok>Malbona princo</lok></fnt>
        <klr>(nun prefere: <tld/>a <ref cel="map.0o">mapo</ref>)</klr>.
        <trd lng="fr">carte de g&eacute;ographie</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Geographie</trd>,
     <trd>Erdkunde</trd></trdgrp>
  <trd lng="fr">g&eacute;ographie</trd>
  <trd lng="ru">&c_g;&c_je;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
</drv>
<drv mrk="geogra1.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Fakulo pri <tld/>o.
    </dif>
    <ref tip="sin" cel="geogra.0o">geografo.</ref>
  </snc>
  <trd lng="de">Geograph</trd>
  <trd lng="fr">g&eacute;ographe</trd>
  <trd lng="ru">&c_g;&c_je;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
</drv>
</art>
