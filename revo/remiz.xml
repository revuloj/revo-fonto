<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: remiz.xml,v 1.23 2024/04/13 20:15:53 revo Exp $">
<kap>
  <rad>remiz</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="remiz.0o">
  <kap><tld/>o</kap>
  <snc mrk="remiz.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Ejo por restigi kaj &scirc;irmi veturilon:
      <ekz>
        mi estis luinta tute le&gcirc;e <tld/>on, sed estis
        &scirc;telinta kamioneton plej kontra&ubreve;le&gcirc;e
        <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;apitro
        7a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="auxt.0ejo">a&ubreve;tejo</ref>,
      <ref cel="garagx.0o">gara&gcirc;o</ref>    
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_a;&c_zh;</trd>
  <trdgrp lng="ca">
    <trd>cobert</trd>,
    <trd>garatge</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>depo</trd>,
    <trd>k&#x16F;lna</trd>,
    <trd>rem&#xED;za</trd>,
    <trd>soukrom&#xE1; gar&#xE1;&#x17E;</trd>,
    <trd>vozovna</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schuppen</trd>,
    <trd>Remise</trd>,
    <trd>Bootshaus</trd>,
    <trd>Garage</trd>
  </trdgrp>
  <trd lng="en">garage</trd>
  <trd lng="es">garaje</trd>
  <trd lng="fr">garage<klr tip="amb"> (p. ex. &agrave; voiture)</klr></trd>
  <trdgrp lng="hu">
    <trd>kocsisz&iacute;n</trd>,
    <trd>rem&iacute;z</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8ECA;&#x5EAB;  <pr>&#x3057;&#x3083;&#x3053;</pr></trd>,
    <trd>&#x30DC;&#x30FC;&#x30C8;&#x5C0F;&#x5C4B;  <pr>&#x307C;&#x30FC;&#x3068;&#x3053;&#x3084;</pr></trd>,
    <trd>&#x99AC;&#x8ECA;&#x5C0F;&#x5C4B;  <pr>&#x3070;&#x3057;&#x3083;&#x3053;&#x3084;</pr></trd>,
    <trd>&#x8ECA;&#x7F6E;&#x304D;&#x5834;  <pr>&#x3057;&#x3083;&#x304A;&#x304D;&#x3070;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>remise</trd>,
    <trd>garage</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>remiza</trd>,
    <trd>zajezdnia</trd>,
    <trd>wozownia</trd>
  </trdgrp>
  <trd lng="pt">garagem</trd>
  <trd lng="ru">&c_g;&c_a;&c_r;&c_a;&c_zh;</trd>
  <trdgrp lng="sk">
    <trd>depo</trd>,
    <trd>rem&#xED;za</trd>,
    <trd>vozov&ncaron;a</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_a;&c_r;&c_je;&c_t;&#x43D;&c_i;&c_j; &c_s;&c_a;&c_r;&c_a;&c_j;</trd>,
    <trd>&c_gu;&c_a;&c_r;&c_a;&c_zh;</trd>,
    <trd>&#x43F;&c_a;&c_r;&#x43A; (&c_t;&c_r;&c_a;&#x43C;&c_v;&c_a;&c_j;&#x43D;&c_i;&c_j;)</trd>,
    <trd>&c_d;&c_je;&#x43F;&#x43E; (&#x43F;&c_a;&c_r;&#x43E;&c_v;&#x43E;&c_z;&#x43D;&c_je;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8ECA;&#x5EAB; <pr>ch&#x113;k&#xF9;</pr></trd>,
    <trd>&#x8F66;&#x5E93; <pr>ch&#x113;k&#xF9;</pr></trd>,
    <trd>&#x8239;&#x68DA; <pr>chu&#xE1;np&#xE9;ng</pr></trd>,
    <trd>&#x8ECA;&#x623F; <pr>ch&#x113;f&#xE1;ng</pr></trd>,
    <trd>&#x8F66;&#x623F; <pr>ch&#x113;f&#xE1;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="remiz.en0igi">
  <kap>en<tld/>igi</kap>
  <snc mrk="remiz.en0igi.KOMUNE">
    <dif>
      &Scirc;irmi, metante en <tld/>on:
      <ekz>
        li <klr>...</klr> en<tld/>igis la riki&scirc;on
        <fnt>
          <aut>Lao She, trad. Wang Chongfang</aut>,
          <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0026/">
          Kamelo &Scirc;jangzi</url></vrk>,
          <lok>1988</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_v;&c_ib;&c_c;&c_mol; &c_u; <ind>&c_g;&c_a;&c_r;&c_a;&c_zh;</ind></trd>,
    <trd>&c_z;&c_a;&c_g;&c_a;&c_n;&c_ja;&c_c;&c_mol; &c_u; <ind>&c_g;&c_a;&c_r;&c_a;&c_zh;</ind></trd>
  </trdgrp>
  <trd lng="fr">mettre au <ind>garage</ind></trd>
</drv>
</art>
