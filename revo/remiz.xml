<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: remiz.xml,v 1.28 2025/08/21 08:38:28 revo Exp $">
<kap>
  <rad>remiz</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="remiz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ARKI</uzo>
  <snc mrk="remiz.0o.veturila">
    <dif>
      Ejo por restigi kaj &scirc;irmi veturilon:
      <ekz>
        mi estis luinta tute le&gcirc;e <tld/>on, sed estis
        &scirc;telinta kamioneton plej kontra&ubreve;le&gcirc;e
        <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;apitro
        7a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="auxt.0ejo">a&ubreve;tejo</ref>,
      <ref cel="garagx.0o">gara&gcirc;o</ref>
    </refgrp>
    <rim>
      La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>gara&gcirc;o</ke>, <ke>veturilejo</ke>.
    </rim>

    <trdgrp lng="be">
      <trd>&c_g;&c_a;&c_r;&c_a;&c_zh;</trd>,
      <trd>&c_a;&c_b;&c_o;&c_z;&c_n;&c_ja;</trd>,
      <trd>&c_v;&c_a;&c_z;&c_o;&c_w;&c_n;&c_ja;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>cobert</trd>,
      <trd>garatge</trd>
    </trdgrp>
    <trdgrp lng="cs">
      <trd>depo</trd>,
      <trd>k&uring;lna</trd>,
      <trd>rem&iacute;za</trd>,
      <trd>soukrom&aacute; gar&aacute;&zcaron;</trd>,
      <trd>vozovna</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Wagenschuppen</trd>,
      <trd>Remise</trd>,
      <trd>Bootshaus</trd>,
      <trd>Garage</trd>
    </trdgrp>
    <trd lng="en">garage</trd>
    <trd lng="es">garaje</trd>
    <trd lng="fr">garage<klr tip="amb"> (p. ex. &agrave; voiture)</klr></trd>
    <trdgrp lng="hu">
      <trd>kocsisz&iacute;n</trd>,
      <trd>rem&iacute;z</trd>
    </trdgrp>
    <trdgrp lng="ja">
      <trd>&#36554;&#24235;  <pr>&#12375;&#12419;&#12371;</pr></trd>,
      <trd>&#12508;&#12540;&#12488;&#23567;&#23627;  <pr>&#12412;&#12540;&#12392;&#12371;&#12420;</pr></trd>,
      <trd>&#39340;&#36554;&#23567;&#23627;  <pr>&#12400;&#12375;&#12419;&#12371;&#12420;</pr></trd>,
      <trd>&#36554;&#32622;&#12365;&#22580;  <pr>&#12375;&#12419;&#12362;&#12365;&#12400;</pr></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>remise</trd>,
      <trd>garage</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>remiza</trd>,
      <trd>zajezdnia</trd>,
      <trd>wozownia</trd>
    </trdgrp>
    <trd lng="pt">garagem</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_a;&c_r;&c_je;&c_t;&c_n;&c_i;&c_k;</trd>,
      <trd>&c_d;&c_je;&c_p;&c_o;</trd>,
      <trd>&c_g;&c_a;&c_r;&c_a;&c_zh;</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>depo</trd>,
      <trd>rem&iacute;za</trd>,
      <trd>vozov&ncaron;a</trd>
    </trdgrp>
    <trdgrp lng="uk">
      <trd>&c_k;&c_a;&c_r;&c_je;&c_t;&c_n;&c_i;&c_j; &c_s;&c_a;&c_r;&c_a;&c_j;</trd>,
      <trd>&c_gu;&c_a;&c_r;&c_a;&c_zh;</trd>,
      <trd>&c_p;&c_a;&c_r;&c_k; (&c_t;&c_r;&c_a;&c_m;&c_v;&c_a;&c_j;&c_n;&c_i;&c_j;)</trd>,
      <trd>&c_d;&c_je;&c_p;&c_o; (&c_p;&c_a;&c_r;&c_o;&c_v;&c_o;&c_z;&c_n;&c_je;)</trd>
    </trdgrp>
    <trdgrp lng="zh">
      <trd>&#36554;&#24235; <pr>ch&emacron;k&ugrave;</pr></trd>,
      <trd>&#36710;&#24211; <pr>ch&emacron;k&ugrave;</pr></trd>,
      <trd>&#33337;&#26842; <pr>chu&aacute;np&eacute;ng</pr></trd>,
      <trd>&#36554;&#25151; <pr>ch&emacron;f&aacute;ng</pr></trd>,
      <trd>&#36710;&#25151; <pr>ch&emacron;f&aacute;ng</pr></trd>
    </trdgrp>
  </snc>
  <snc mrk="remiz.0o.sxedo">
    <dif>
      Aparta nelo&gcirc;ata konstrua&jcirc;o por teni diversajn hava&jcirc;ojn (ilojn,
      hejtlignon, malnovajn meblojn ktp):
      <ekz>
        Ni kune iras al mia <tld/>o, kaj vi &jcirc;etas rigardon enen:
        &scirc;tupetaro, malplenaj farbujoj, malnova triciklo &ndash; sed neniu
        drako
        <fnt><bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/wiki/La_drako_en_la_remizo"
               >La drako en la remizo</url></lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="sxed.0o">&scirc;edo</ref>
    <trdgrp lng="be">
      <trd>&c_p;&c_u;&c_n;&c_ja;</trd>,
      <trd>&c_d;&c_r;&c_y;&c_v;&c_o;&c_t;&c_n;&c_ib;&c_k;</trd>,
      <trd>&c_p;&c_a;&c_d;&c_p;&c_a;&c_v;&c_je;&c_t;&c_k;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Ger&auml;teschuppen</trd>,
      <trd>Werkzeugschuppen</trd>
    </trdgrp>
    <trd lng="en">shed</trd>
    <trd lng="ru">&c_s;&c_a;&c_r;&c_a;&c_j;</trd>
  </snc>
</drv>

<drv mrk="remiz.en0igi">
  <kap>en<tld/>igi</kap>
  <snc mrk="remiz.en0igi.KOMUNE">
    <dif>
      &Scirc;irmi, metante en <tld/>on:
      <ekz>
        li <klr>...</klr> en<tld/>igis la riki&scirc;on
        <fnt>
          <aut>Lao She, trad. Wang Chongfang</aut>,
          <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0026/">
          Kamelo &Scirc;jangzi</url></vrk>,
          <lok>1988</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_v;&c_ib;&c_c;&c_mol; &c_u; <ind>&c_g;&c_a;&c_r;&c_a;&c_zh;</ind></trd>,
    <trd>&c_z;&c_a;&c_g;&c_a;&c_n;&c_ja;&c_c;&c_mol; &c_u; <ind>&c_g;&c_a;&c_r;&c_a;&c_zh;</ind></trd>
  </trdgrp>
  <trd lng="de">im Schuppen <ind>abstellen</ind></trd>
  <trd lng="fr">mettre au <ind>garage</ind></trd>
</drv>
</art>
