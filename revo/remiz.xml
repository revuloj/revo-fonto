<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>remiz</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="remiz.0o">
  <kap><tld/>o</kap>
  <snc mrk="remiz.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Ejo por restigi kaj &scirc;irmi veturilon:
      <ekz>
        mi estis luinta tute le&gcirc;e <tld/>on, sed estis &scirc;telinta kamioneton plej kontra&ubreve;le&gcirc;e
        <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;apitro 7a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="auxt.0ejo">a&ubreve;tejo</ref>,
    <ref cel="garagx.0o">gara&gcirc;o</ref>    
  </refgrp>
  <trd lng="be">&c_g;&c_a;&c_r;&c_a;&c_zh;</trd>
  <trd lng="de">Garage</trd>
  <trd lng="en">garage</trd>
  <trd lng="es">garaje</trd>
  <trdgrp lng="fr">
    <trd>garage<klr tip="amb"> (pour une voiture)</klr></trd>,
    <trd>remise<klr tip="amb"> (garage)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kocsisz&iacute;n</trd>,
    <trd>rem&iacute;z</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>remise</trd>,
    <trd>garage</trd>
  </trdgrp>
  <trd lng="pt">garagem</trd>
  <trd lng="ru">&c_g;&c_a;&c_r;&c_a;&c_zh;</trd>
</drv>
</art>
