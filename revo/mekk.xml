<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: mekk.xml,v 1.9 2022/05/01 12:05:12 revo Exp $">
    <kap>
      <rad>Mekk</rad>/o
        <fnt>
          <bib>FK</bib>
        </fnt>,
        <var><kap><rad var="k">Mek</rad>/o</kap></var>
    </kap>

<drv mrk="mekk.0o">
  <kap><tld/>o, <var><kap><tld var="k"/>o</kap></var></kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">ISL</uzo>
    <dif>
      La sankta <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      de <ref tip="malprt" cel="islam.0o">islamo</ref> en la okcidento
      de
      <ref tip="lst" lst="voko:urboj" cel="arab.SaudaA0ujo">Sauda
      Arabujo</ref>:
      <ekz>
        la tabako estas la plezuro de la had&jcirc;ioj, kiuj voja&gcirc;as al
        <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        en lia epoko en <tld/>o oni pruntedonis monon kontra&ubreve; vere
        rabema interezo
        <fnt>
          <bib>Monato</bib>
          <aut>Hussain Al-Amily</aut>
          <vrk><url ref="https://www.monato.net/2015/011446.php">Islama ekonomio: &ccirc;u modelo por hodia&ubreve;?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        tio venas de araba ha&gcirc;: pilgrimisto al <tld var="k"/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url ref="&Steloj;monato2/006903.html">Pruntoj: Arabaj vortoj en la germana kaj esperanta
              lingvoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_M;&c_e;&c_k;&c_a;</trd>
    <trd lng="de">Mekka</trd>
    <trd lng="fr">La <ind>Mecque</ind></trd>
    <trd lng="id">Mekkah</trd>
    <trd lng="pl">Mekka</trd>     
    <trd lng="ro">Mecca</trd>
  </snc>
  <trd lng="uk">&#x41C;&#x435;&#x43A;&#x43A;&#x430;</trd>
</drv>

  </art>

