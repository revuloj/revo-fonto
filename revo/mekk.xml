<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: mekk.xml,v 1.13 2024/01/25 20:10:36 revo Exp $">
    <kap>
      <rad>Mekk</rad>/o
        <fnt>
          <bib>FK</bib>
        </fnt>,
        <var><kap><rad var="k">Mek</rad>/o</kap></var>
    </kap>

<drv mrk="mekk.0o">
  <kap><tld/>o, <var><kap><tld var="k"/>o</kap></var></kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">ISL</uzo>
    <dif>
      La sankta <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      de <ref tip="malprt" cel="islam.0o">islamo</ref> en la okcidento
      de
      <ref tip="lst" lst="voko:urboj" cel="arab.SaudaA0ujo">Sauda
      Arabujo</ref>:
      <ekz>
        la tabako estas la plezuro de la had&jcirc;ioj, kiuj voja&gcirc;as al
        <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        en lia epoko en <tld/>o oni pruntedonis monon kontra&ubreve; vere
        rabema interezo
        <fnt>
          <bib>Monato</bib>
          <aut>Hussain Al-Amily</aut>
          <vrk><url ref="&Monato;/2015/011446.php">Islama ekonomio: &ccirc;u modelo por
          hodia&ubreve;?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        tio venas de araba ha&gcirc;: pilgrimisto al <tld var="k"/>o
       <fnt>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2000/006903.php">Arabaj vortoj en la germana
kaj esperanta lingvoj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 15</lok>
          </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_M;&c_e;&c_k;&c_a;</trd>
    <trd lng="de">Mekka</trd>
    <trd lng="fr">La <ind>Mecque</ind></trd>
    <trd lng="id">Mekkah</trd>
    <trd lng="nl">Mekka</trd>
    <trd lng="pl">Mekka</trd>     
    <trd lng="ro">Mecca</trd>
  </snc>
  <trd lng="ja">&#x30E1;&#x30C3;&#x30AB;</trd>
  <trd lng="uk">&#x41C;&c_je;&#x43A;&#x43A;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x9EA5;&#x52A0; <pr>M&#xE0;iji&#x101;</pr></trd>,
    <trd>&#x9EA6;&#x52A0; <pr>M&#xE0;iji&#x101;</pr></trd>
  </trdgrp>
</drv>

  </art>

