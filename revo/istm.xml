<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>istm</rad>/o
</kap>
<drv mrk="istm.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Mallar&gcirc;a strio de tero inter du maroj, 
      kuniganta du terpartojn:
      <ekz><tld/>o de Panama, de Suez.</ekz>
    </dif>
    <ref tip="sin" cel="kol.ter0o">terkolo</ref>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_sh;&c_y;&c_je;&c_k;</trd>
  <trd lng="ca">istme</trd>
  <trdgrp lng="de">
    <trd>Isthmus</trd>,
    <trd>Landenge</trd>
  </trdgrp>
  <trd lng="es">istmo</trd>
  <trd lng="fr">isthme</trd>
  <trdgrp lng="hu">
    <trd>f&ouml;ldszoros</trd>,
    <trd>isztmosz</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>isthmus</trd>,
    <trd>landengte</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_sh;&c_je;&c_je;&c_k;</trd>
</drv>
</art>
