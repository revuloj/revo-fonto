<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: istm.xml,v 1.20 2019/10/12 11:10:20 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>istm</rad>/o
</kap>
<drv mrk="istm.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="istm.0o.terkolo">
    <dif>
      <ref tip="dif" cel="kol.ter0o">Terkolo</ref>:
      <ekz>
        homfarita kanalo tra la panama <tld/>o, por la internacia trafiko de &scirc;ipoj inter
        oceanoj Atlantiko kaj Pacifiko
        <fnt>
          <vrk><url ref="https://www.youtube.com/watch?v=vZ1pwetKZ9s"
              >Esperanto - Panama kanalo</url></vrk>
          <lok>Jutubo</lok>
        </fnt>;
      </ekz>
      <ekz>
        Centra Ameriko estas longa <tld/>o, kiu kuras ekde Jukatano en suda Meksiko &gcirc;is
        la Panama kanalo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2865"
              >Centra Ameriko</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_sh;&c_y;&c_je;&c_k;</trd>
  <trd lng="ca">istme</trd>
  <trdgrp lng="de">
    <trd>Isthmus</trd>,
    <trd>Landenge</trd>
  </trdgrp>
  <trd lng="en">isthmus</trd>
  <trd lng="es">istmo</trd>
  <trd lng="fr">isthme</trd>
  <trdgrp lng="hu">
    <trd>f&ouml;ldszoros</trd>,
    <trd>isztmosz</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>isthmus</trd>,
    <trd>landengte</trd>
  </trdgrp>
  <trd lng="pt">istmo<klr tip="amb"> (geografia)</klr></trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_sh;&c_je;&c_je;&c_k;</trd>
  <trd lng="sk">zemsk&aacute; &uacute;&zcaron;ina</trd>
  <trdgrp lng="cs">
    <trd>istmus</trd>,
    <trd>pevninsk&aacute; &scaron;&iacute;je</trd>
  </trdgrp>
</drv>
</art>
