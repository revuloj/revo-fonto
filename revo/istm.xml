<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>istm</rad>/o
</kap>
<drv mrk="istm.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Mallar&gcirc;a terkolo inter du maroj, kuniganta du terpartojn:
      <ekz><tld/>o de Panama, de Suez.</ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Isthmus</trd>,
    <trd>Landenge</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>isthmus</trd>,
    <trd>landengte</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_sh;&c_je;&c_je;&c_k;</trd>
</drv>
</art>
