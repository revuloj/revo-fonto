<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>doz</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="doz.0o">
  <kap><tld/>o</kap>
  <snc mrk="doz.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Difinita <ref tip="vid" cel="kvant.0o">kvanto</ref> de 
      medikamento:
      <ekz>
        provu unue malgrandajn <tld/>ojn.
      </ekz>
    </dif>
  </snc>
  <snc mrk="doz.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Difinita kvanto de io:
      <ekz>
        ioma <tld/>o de poezio<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &scirc;i posedas sufi&ccirc;an <tld/>on de prudento
        por kompreni;
      </ekz>
      <ekz>
        &ccirc;e plej malgranda <tld/>o da honesteco kaj bona
        volo<fnt>Z</fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_o;&c_z;&c_a;</trd>
  <trd lng="de">Dosis</trd>
  <trd lng="es">dosis</trd>
  <trd lng="fr">dose</trd>
  <trdgrp lng="pl">
    <trd>doza</trd>,
    <trd>dawka</trd>
  </trdgrp>
  <trd lng="pt">dose</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_z;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="doz.tro0o">
  <kap>tro<tld/>o, <var><kap>super<tld/>o</kap></var></kap>
  <snc mrk="doz.tro0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Tro granda <tld/>o, eventuale dan&gcirc;era:
      <ekz>
        en la a&gcirc;o de 36 jaroj &scirc;i mortis pro tro<tld/>o da
        dormigiloj
        <fnt>
        <bib>Viki</bib>,
        <lok><url ref="http://eo.wikipedia.org/wiki/Marilyn_Monroe">
        Marilyn Monroe</url>,
        2009-12-04</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la artistoj ja estis tre, tre bonaj, sed &ccirc;iuj programeroj
        estis
        en si mem plenaj koncertoj. Do, estis super<tld/>o, kaj homoj
        tedi&gcirc;is
        <fnt>
        <aut>Judith</aut>,
        <vrk>lumo</vrk>,
        <lok><url
        ref="http://parelie.wordpress.com/2009/09/23/bjalistoka-kulturmento/">
        2009-09-23</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>overdose</trd>,
    <trd>surdosage</trd>
  </trdgrp>
</drv>

<drv mrk="doz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="doz.0i.MED">
    <dif>
      Mezuri kaj prepari la &gcirc;ustan kvanton de iu medikamento:
      <ekz>
        oni devas tre prudente <tld/>i la venenajn kuracilojn.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">dosieren</trd>
  <trd lng="fr">doser</trd>
  <trd lng="es">dosificar</trd>
  <trdgrp lng="pl">
    <trd>dozowa&cacute;</trd>,
    <trd>dawkowa&cacute;</trd>
  </trdgrp>
  <trd
  lng="ru">&c_d;&c_o;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
