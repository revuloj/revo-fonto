<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: brauxning.xml,v 1.7 2023/01/30 09:28:14 revo Exp $">
<kap>
  <rad>bra&ubreve;ning</rad>/o
</kap>
<drv mrk="brauxning.0o">
  <kap><tld/>o</kap>
  <snc mrk="brauxning.0o.ARM">
    <dif>
      Duona&ubreve;tomata pafilo, precipe <ref tip="super" cel="pistol.0o.ARM">pistolo</ref>, funkcianta
      la&ubreve; procedo 
      inventita de samnomulo:
      <ekz>
        la kapitano pafis el <tld/>o direkte al la boato
        <fnt>
          <bib>Salamandroj</bib>,
          <lok>libro 1a, &ccirc;apitro 1a</lok>
        </fnt>;
      </ekz>
      <ekz>
        neniu reciprokis el ia ajn <tld/>o, oni pafis al vaka loko
        <fnt><bib>MkM</bib>, <lok>epilogo</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_r;&c_a;&c_w;&c_n;&c_ib;&c_n;&c_g;</trd>
  <trd lng="bg">&c_b;&c_r;&c_o;&c_u;&c_n;&c_i;&c_n;&c_g;</trd>
  <trd lng="de">Browning<klr tip="amb"> (Schusswaffe)</klr></trd>
  <trd lng="fa">&a_b;&a_r;&a_w;&a_n;&f_y;&a_n;&f_g;<klr tip="ind"> (&a_t;&f_p;&a_A;&a_n;&f_ch;&a_h;)</klr></trd>
  <trd lng="fr">browning<klr> (arme)</klr></trd>
  <trdgrp lng="pl">
    <trd>browning</trd>,
    <trd>karabin Browninga</trd>
  </trdgrp>
  </snc>
  <trd lng="uk">&c_b;&c_r;&c_a;&c_u;&#x43D;&c_ib;&#x43D;&c_gu;</trd>
</drv>
</art>

