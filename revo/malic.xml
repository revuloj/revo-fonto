    <trd>malvado</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>perfide</trd>,
    <trd>m&eacute;chant</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x60AA;&#x610F;&#x306E;  <pr>&#x3042;&#x304F;&#x3044;&#x306E;</pr></trd>,
    <trd>&#x610F;&#x5730;&#x60AA;&#x306A;  <pr>&#x3044;&#x3058;&#x308F;&#x308B;&#x306A;</pr></trd>,
    <trd>&#x90AA;&#x60AA;&#x306A;  <pr>&#x3058;&#x3083;&#x3042;&#x304F;&#x306A;</pr></trd>,
    <trd>&#x8179;&#x9ED2;&#x3044;  <pr>&#x306F;&#x3089;&#x3050;&#x308D;&#x3044;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>boos</trd>,
    <trd>boosaardig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>podst&eogonek;pny</trd>,
    <trd>przewrotny</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>malicioso</trd>,
    <trd>malvado</trd>,
    <trd>maldoso</trd>,       
    <trd>perverso</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_l;&c_o;&c_b;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_z;&c_l;&c_o;&c_j;</trd>,
    <trd>&c_k;&c_o;&c_v;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>zlomyse&lcaron;n&yacute;</trd>,
    <trd>&scaron;kodoradostn&yacute;</trd>
  </trdgrp>
  <trd lng="sv">elak</trd>
  <trd lng="tok">ike</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&#x43B;&c_i;&c_j;</trd>,
    <trd>&c_z;&#x43B;&#x43E;&c_s;&c_t;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_z;&#x43B;&#x43E;&c_s;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_z;&#x43B;&c_ib;&c_s;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_ib;&c_d;&c_s;&c_t;&c_u;&#x43F;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6076;&#x6BD2; <pr>&#xE8;d&#xFA;</pr></trd>,
    <trd>&#x60E1;&#x6BD2; <pr>&#xE8;d&#xFA;</pr></trd>,
    <trd>&#x9669;&#x6076; <pr>xi&#x1CE;n&#xE8;</pr></trd>,
    <trd>&#x96AA;&#x60E1; <pr>xi&#x1CE;n&#xE8;</pr></trd>,
    <trd>&#x72E0;&#x6BD2; <pr>h&#x11B;nd&#xFA;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="malic.0eta">
  <kap><tld/>eta</kap>
  <snc mrk="malic.0eta.inklina">
    <dif>
      Inklina iom mokpetoli sen deziro malutili:
      <ekz>
        <tld/>eta infano.
      </ekz>
    </dif>
  </snc>
  <snc mrk="malic.0eta.montranta">
    <dif>
      Montranta mokpetolemon sen deziro malutili:
      <ekz>
        <tld/>eta rido, okulo, &scirc;erco;
      </ekz>
      <ekz>
        liaj artikoloj, spicitaj per <tld/>etaj komentoj, estis amuza lega&jcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2009/009914.php">Gazet-eraroj</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
     <ref cel="mok.0ema">mokema</ref>,
     <ref cel="amik.mal0o">malamik</ref>a,
     <ref cel="incit.0i">inciti</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_r;&c_e;&c_z;&c_mol;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_k;&c_je;&c_p;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">&scaron;k&aacute;dliv&yacute;</trd>
  <trdgrp lng="de">
    <trd>garstig</trd>,
    <trd>gemein</trd>,
    <trd>zeckend</trd>
  </trdgrp>
  <trd lng="en">ill-natured</trd>
  <trd lng="fr">malicieux</trd>
  <trdgrp lng="hu">
    <trd>huncut</trd>,
    <trd>gonoszkod&oacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3044;&#x305F;&#x305A;&#x3089;&#x306A;</trd>,
    <trd>&#x304B;&#x3089;&#x304B;&#x3044;&#x306E;</trd>
  </trdgrp>
  <trdgrp lng="nl">
