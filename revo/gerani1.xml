<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gerani1.xml,v 1.14 2023/11/11 10:00:40 revo Exp $">
<kap>
  <rad>geraniac</rad>/oj
</kap>
<drv mrk="gerani1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Geraniaceae</trd>):
      <ekz>
        <tld/>oj <klr>[&#x2026;]</klr> estas herbaj plantoj a&ubreve; arbetoj kiuj produktas
        esenciajn oleojn en kontinentaj &gcirc;is tropikaj zonoj, 
        <ref tip="prt" cel="gerani.0o">geranio</ref>
        kaj pelargonio estas tre uzataj en hortikulturo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=20453">Geraniacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_r;&c_a;&c_n;&c_je;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Storchschnabelgew&auml;chse</trd>
  <trdgrp lng="en">
    <trd>Geraniaceae</trd>,
    <trd>geranium family</trd>
  </trdgrp>
  <trd lng="fr">g&eacute;raniac&eacute;es</trd>
  <trd lng="hu">g&oacute;lyaorrf&eacute;l&eacute;k</trd>
  <trd lng="nl">ooievaarsbekfamilie</trd>
  <trd lng="pl">bodziszkowate</trd>
  <trd lng="pt">gerani&aacute;ceas</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_je;&c_r;&c_a;&c_n;&c_i;&c_je;&c_v;&c_y;&c_je;</trd>,
    <trd>&c_g;&c_je;&c_r;&c_a;&c_n;&c_je;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="uk">&#x433;&#x435;&#x440;&#x430;&#x43D;&#x456;&#x454;&#x432;&#x456;</trd>
  <trd lng="zh">&#x8001;&#x9E1B;&#x8349; <pr>l&#x1CE;ogu&#xE0;nc&#x1CE;o</pr></trd>
</drv>
</art>
