    <trd>&#x53B3;&#x683C;&#x306A;  <pr>&#x3052;&#x3093;&#x304B;&#x304F;&#x306A;</pr></trd>,
    <trd>&#x5BB9;&#x8D66;&#x306E;&#x306A;&#x3044;  <pr>&#x3088;&#x3046;&#x3057;&#x3083;&#x306E;&#x306A;&#x3044;</pr></trd>,
    <trd>&#x904E;&#x9177;&#x306A;  <pr>&#x304B;&#x3053;&#x304F;&#x306A;</pr></trd>,
    <trd>&#x731B;&#x70C8;&#x306A;  <pr>&#x3082;&#x3046;&#x308C;&#x3064;&#x306A;</pr></trd>,
    <trd>&#x6DF1;&#x523B;&#x306A;  <pr>&#x3057;&#x3093;&#x3053;&#x304F;&#x306A;</pr></trd>,
    <trd>&#x7C21;&#x7D20;&#x306A;  <pr>&#x304B;&#x3093;&#x305D;&#x306A;</pr></trd>,
    <trd>&#x7121;&#x99C4;&#x306E;&#x306A;&#x3044;  <pr>&#x3080;&#x3060;&#x306E;&#x306A;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">streng</trd>
  <trdgrp lng="pl">
    <trd>srogi</trd>,
    <trd>surowy</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_r;&c_o;&c_g;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_u;&c_r;&c_o;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">pr&iacute;sny</trd>
