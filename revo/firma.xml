<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>firma</rad>/o
</kap>
<uzo tip="fak">EKON</uzo>
<drv mrk="firma.0o">
  <kap><tld/>o</kap>
  <snc>
    <ref tip="dif" cel="firm1.0o.entrepreno">firmo</ref>
    <ekz>
      kostoj de la transiro al e&ubreve;roj por <tld/>oj kaj publikaj
      servoj<fnt><aut>Marc Vanden Bempt</aut>: <vrk>E&ubreve;ro: ecoj kaj sekvoj
      por &ccirc;iuj</vrk>, <bib>Monato</bib><lok>jaro 1998a, numero 9a, p.
      17a</lok></fnt>;
    </ekz>
    <ekz>
      plej multaj retservoj simple revendas sub sia aparta nomo la servon
      de pli granda ret- a&ubreve; telefon-<tld/>o<fnt><aut>Edmund Grimley
      Evans</aut>: <vrk>Interreto senpage</vrk>,
      <bib>Monato</bib><lok>jaro 2000a, numero 11a, p. 23a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_ib;&c_r;&c_m;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_d;&c_p;&c_r;&c_y;&c_je;&c_m;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_m;&c_p;&c_a;&c_n;&c_ib;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Firma</trd>,
    <trd>Unternehmen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>firme</trd>,
    <trd>entreprise<klr> (soci&eacute;t&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="hu">c&eacute;g</trd>
  <trd lng="nl">firma</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_i;&c_r;&c_m;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="firma.et0o">
  <kap>et<tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <ref tip="dif" cel="firm1.0eto">firmeto</ref>
    <ekz>
      la et<tld/>oj, kiu fakte faras la laboron<fnt><aut>Paul
      Gubbins</aut>: <vrk>Politiko elreligita</vrk>,
      <bib>Monato</bib><lok>jaro 2001a, numero 2a, p. 6a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_o;&c_je; <ind>&c_p;&c_r;&c_a;&c_d;&c_p;&c_r;&c_y;&c_je;&c_m;&c_s;&c_t;&c_v;&c_a;</ind></trd>
  <trdgrp lng="de">
    <trd>Kleinbetrieb</trd>,
    <trd>Kleinunternehmen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>petite entreprise</trd>,
    <trd>PME</trd>
  </trdgrp>
  <trd lng="hu">kis <ind>c&eacute;g</ind></trd>
  <trd lng="ru">&c_m;&c_a;&c_l;&c_o;&c_je; <ind>&c_p;&c_r;&c_je;&c_d;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_i;&c_je;</ind></trd>
</drv>
</art>
