<art mrk="$Id: psikia1.xml,v 1.24 2024/02/17 11:11:10 revo Exp $">
<kap>
  <rad>psikiatri</rad>/o <fnt>Z</fnt>,
  <var><kap><rad>psi&hcirc;iatri</rad>/o</kap></var>
</kap>
<drv mrk="psikia1.0o">
  <kap><tld/>o,
    <var><kap>psi&hcirc;iatrio</kap></var>
  </kap>
  <snc mrk="psikia1.0o.MED">
  <uzo tip="fak">MED</uzo>
  <uzo tip="fak">PSI</uzo>
    <dif>
      <ref tip="malprt" cel="medici.0o">Medicina</ref> fako, 
      kiu studas la <ref tip="prt" cel="psik.0o">psikajn</ref>
      perturbojn kaj ties kuracadon:
      <ekz>
        la propono priskribis sistemon de volonta e&ubreve;tanazio administrata de kuracisto post konsento
        de du aliaj kuracistoj, kaj post <tld/>a taksado, konsilado kaj pripensado de la petinto
        <fnt>
          <bib>Monato</bib>
          <aut>D. E. ROGERS</aut>
          <vrk><url ref="&Monato;/2003/008188.html">Jes kaj ne</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        okcidentaj veteranoj de la milito ankora&ubreve; suferegas pro malsanoj korpaj kaj, plej grave,
        <tld/>aj, okazigitaj de tiu milito
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2005/008073.php">Popolo suferinta pro agresemo de
          aliaj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="psikia.0o">psikiatro</ref>
  </snc>
  <trd lng="be">&c_p;&c_s;&c_ib;&c_h;&c_ib;&c_ja;&c_t;&c_r;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_p;&c_s;&c_i;&c_h;&c_i;&c_a;&c_t;&c_r;&c_i;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>l&eacute;&ccaron;en&iacute; nervov&yacute;ch chorob</trd>,
    <trd>psychiatrie</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Psychiatrie</trd>,
    <trd>Nervenheilkunde</trd>
  </trdgrp>
  <trd lng="en">psychiatry</trd>
  <trd lng="fa">&a_r;&a_w;&a_A;&a_n;&f_p;&a_z;&a_sh;&f_k;&f_y;</trd>
  <trd lng="fr">psychiatrie</trd>
  <trd lng="fy">psychiatry</trd>
  <trdgrp lng="hu">
    <trd>pszichi&aacute;tria</trd>,
    <trd>elmek&oacute;rtan</trd>,
    <trd>elmegy&oacute;gy&aacute;szat</trd>
  </trdgrp>
  <trd lng="nl">psychiatrie</trd>
  <trd lng="pl">psychiatria</trd>
  <trd lng="pt">psiquiatria</trd>
  <trd lng="ru">&c_p;&c_s;&c_i;&c_h;&c_i;&c_a;&c_t;&c_r;&c_i;&c_ja;</trd>
  <trd lng="sk">psychiatria</trd>
  <trdgrp lng="zh">
    <trd>&#x7CBE;&#x795E;&#x79D1; <pr>j&#x12B;ngsh&#xE9;nk&#x113;</pr></trd>,
    <trd>&#x8EAB;&#x5FC3;&#x79D1; <pr>sh&#x113;nx&#x12B;nk&#x113;</pr></trd>,
    <trd>&#x7CBE;&#x795E;&#x75C5;&#x5B66; <pr>j&#x12B;ngsh&#xE9;nb&#xEC;ngxu&#xE9;</pr></trd>,
    <trd>&#x7CBE;&#x795E;&#x75C5;&#x5B78; <pr>j&#x12B;ngsh&#xE9;nb&#xEC;ngxu&#xE9;</pr></trd>
  </trdgrp>
</drv>
