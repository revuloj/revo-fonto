<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>amfor</rad>/o
</kap>
<drv mrk="amfor.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Antikva alta botelego el argilo kun pinta bazo kaj kun du teniloj,
      por konservi vinon, oleon.
    </dif>
  </snc>
  <trd lng="en">amphora</trd>
  <trd lng="fr">amphore</trd>
  <trd lng="hu">amfora</trd>
  <trd lng="nl">amfoor</trd>
  <trd lng="ru">&c_a;&c_m;&c_f;&c_o;&c_r;&c_a;</trd>
</drv>
</art>
