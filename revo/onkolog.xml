<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>onkolog</rad>/o
</kap>
<drv mrk="onkolog.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="onkolog.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="dif" cel="kancerolog.0o.MED">Kancerologo</ref>:
      <ekz>
        hodia&ubreve; fini&gcirc;is en <nom>Du&scirc;anbe</nom> 6a Internacia kongreso de kirurgoj, 
        <tld/>oj kaj radiologoj
        <fnt>
          <aut>S. Saidilhomzoda</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/23187/276276/comment/12044571#comment12044571">
          toleremo</url></vrk>,
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj konstatis, ke komencis disvasti&gcirc;i tra lia korpo
        metastazoj
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Patrick_Swayze">
          Patrick Swayze</url>, 2011-11-22</lok>
        </fnt>.
    </ekz>
    </dif>
    <trd lng="de">Onkologe</trd>
  </snc>
  <trd lng="ca">onc&ograve;leg</trd>
  <trd lng="fr">oncologue</trd>
</drv>
</art>

