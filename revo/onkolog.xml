<art mrk="$Id: onkolog.xml,v 1.8 2024/01/30 16:09:26 revo Exp $">
<kap>
  <rad>onkolog</rad>/o
</kap>
<drv mrk="onkolog.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="onkolog.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="dif" cel="kancerolog.0o.MED">Kancerologo</ref>:
      <ekz>
        hodia&ubreve; fini&gcirc;is en <nom>Du&scirc;anbe</nom> 6a Internacia kongreso de kirurgoj, 
        <tld/>oj kaj radiologoj
        <fnt>
          <aut>S. Saidilhomzoda</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/23187/276276/comment/12044571#comment12044571">
          toleremo</url></vrk>,
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj konstatis, ke komencis disvasti&gcirc;i tra lia korpo
        metastazoj
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Patrick_Swayze">
          Patrick Swayze</url>, 2011-11-22</lok>
        </fnt>.
    </ekz>
    </dif>
    <trd lng="de">Onkologe</trd>
    <trdgrp lng="zh">
    <trd>&#x80BF;&#x7624;&#x533B;&#x5E08; <pr>zh&#x1D2;ngli&#xFA;y&#x12B;sh&#x12B;</pr></trd>,
    <trd>&#x816B;&#x7624;&#x91AB;&#x5E2B; <pr>zh&#x1D2;ngli&#xFA;y&#x12B;sh&#x12B;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_n;&c_k;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="ca">onc&ograve;leg</trd>
  <trd lng="fr">oncologue</trd>
  <trd lng="pl">onkolog</trd>
  <trd lng="ro">oncolog</trd>
</drv>
</art>

