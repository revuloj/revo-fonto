<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">

<kap>
  <ofc>*</ofc>
  <rad>dung</rad>/i
</kap>

<drv mrk="dung.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">JUR</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Interkonsenti kun laboristo, servisto a&ubreve; oficisto por
      havigi al si lian servadon la&ubreve; difinita salajro:
      <ekz><tld/>i vartistinon;</ekz>
      <ekz>asekuri siajn <tld/>itojn kontra&ubreve; akcidentoj;</ekz>
      <ekz>mi mal<tld/>is lin pro malhonesteco.</ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_j;&c_m;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_n;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>einstellen</trd>,
     <trd>dingen</trd>,
     <trd>anheuern</trd>,
     <trd>in Dienst nehmen</trd>,
     <trd>in Lohn nehmen</trd>
  </trdgrp>
  <trd lng="en">hire</trd>
  <trd lng="es">contratar</trd>
  <trd lng="fr">embaucher</trd>
  <trdgrp lng="hu">
    <trd>alkalmaz</trd>,
    <trd>felvesz</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>empregar</trd>,
    <trd>contratar</trd>,
    <trd>assaliar</trd>,
    <trd>engajar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="dung.sin0igi">
  <kap>sin <tld/>igi</kap>
  <snc>
    <dif>
      Interkonsenti kun <tld/>anto.  
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_j;&c_m;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><klr>sich </klr>verdingen</trd>,
    <trd>anheuern</trd>,
    <trd>einstellen lassen</trd>,
    <trd>eingestellt werden</trd>
  </trdgrp>
  <trd lng="fr">se faire <ind>embaucher</ind></trd>
  <trd lng="hu">elhelyezkedk<klr tip="ind"> (&aacute;ll&aacute;sban)</klr></trd>
  <trd lng="ru">&c_n;&c_a;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="dung.0ito">
  <kap><ofc>*</ofc><tld/>ito</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Laboristo <tld/>ita.
    </dif>
  </snc>
  <trd lng="bg">&c_s;&c_l;&c_u;&c_zh;&c_je;&c_shch;</trd>
  <trdgrp lng="de">
    <trd>Angestellter</trd>
  </trdgrp>
  <trd lng="en">employee</trd>
</drv>
</art>
