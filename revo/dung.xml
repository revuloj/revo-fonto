<drv mrk="dung.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">JUR</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Interkonsenti kun laboristo, servisto a&ubreve; oficisto por
      havigi al si ties servadon la&ubreve; difinita salajro:
      <ekz>
        mi devas <tld/>i bonan advokaton kaj mi tute ne havas monon
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is servistojn kaj a&ccirc;etis &ccirc;ion necesan por longa voja&gcirc;o
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>is kontra&ubreve; vi Bileamon, <klr>[&#8230;]</klr> por malbeni vin
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 23:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>is al si tridek du mil &ccirc;arojn kaj la re&gcirc;on de <nom>Maa&hcirc;a</nom> <klr>[&#8230;]</klr> kaj
        venis al la milito
        <fnt>
          <bib>MT</bib>
          <lok>I. Kroniko 19:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i laboristojn por sia vinber&gcirc;ardeno
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 20:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        en tiu urbo fra&ubreve;lino Delanjo <tld/>is kale&scirc;on
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni <tld/>is &scirc;ipon kaj virojn por &gcirc;in veturigi
        <fnt>
          <bib>Rob</bib>
          <lok>Parto V</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u ta&ubreve;gan virinon por la mastrumado
        <fnt>
          <bib>HsT</bib>
          <lok>Heredanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        Kristoforo <tld/>is maljunulon por ru&gcirc;kolori la konstrua&jcirc;on kaj forigi la
        malpura&jcirc;on
        <fnt>
          <bib>HsT</bib>
          <lok>Feli&ccirc;a homo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> pograndistoj <tld/>is kolportistojn, kiuj devis kvitanci liston
        de la vendotaj komerca&jcirc;oj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="posten.en0igi">enpostenigi</ref>,
      <ref cel="lu.0i">lui</ref>,
      <ref cel="okup.0i.laborigi">okupi<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_j;&c_m;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_n;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_n;&c_a;&c_je;&c_m;&c_a;&c_m; &c_n;&c_a; &c_r;&c_a;&c_b;&c_o;&c_t;&c_a;</trd>
  <trd lng="bo">&t_ga;&t_yata;&t_ra;&t_tsheg;&t_ba;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>najmout do pr&aacute;ce</trd>,
    <trd>p&rcaron;ijmout do zam&ecaron;stn&aacute;n&iacute;</trd>,
    <trd>zam&ecaron;stnat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>einstellen</trd>,
    <trd>dingen</trd>,
    <trd>anheuern</trd>,
    <trd>in <ind>Dienst</ind> nehmen</trd>,
    <trd>in <ind>Lohn</ind> nehmen</trd>,
    <trd>anstellen</trd>,
    <trd>verdingen</trd>
  </trdgrp>
  <trd lng="en">hire</trd>
  <trd lng="es">contratar</trd>
  <trdgrp lng="fr">
    <trd>embaucher</trd>
    <trd>engager<klr tip="ind"> (embaucher)</klr></trd>
    <trd>prendre<klr tip="ind"> (embaucher)</klr></trd>
    <trd>recruter<klr tip="ind"> (embaucher)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>alkalmaz</trd>,
    <trd>felvesz</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#38599;&#12358;  <pr>&#12420;&#12392;&#12358;</pr></trd>,
    <trd>&#25505;&#29992;&#12377;&#12427;  <pr>&#12373;&#12356;&#12424;&#12358;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>inhuren</trd>,
    <trd>in <ind>dienst</ind> nemen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zatrudnia&cacute;</trd>,
    <trd>najmowa&cacute;</trd>,
    <trd>przyjmowa&cacute;<klr> do pracy</klr></trd>,
    <trd>anga&zdot;owa&cacute;<klr> do pracy</klr><klr>
    (przest.)</klr></trd>,
    <trd>podpisa&cacute; umow&eogonek; o prac&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>empregar</trd>,
    <trd>contratar</trd>,
    <trd>assaliar</trd>,
    <trd>engajar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>naja&tcaron;</trd>,
    <trd>zamestna&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_n;&c_a;&c_j;&c_m;&c_a;&c_t;&c_i; (&c_k;&c_o;&c_g;&c_o;-&c_n;.)</trd>
  <trdgrp lng="zh">
    <trd>&#25307;&#25910; <pr>zh&amacron;osh&#333;u</pr></trd>,
    <trd>&#25910;&#24405; <pr>sh&#333;ul&ugrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="dung.0anto">
  <kap><tld/>anto</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Iu, kiu <tld/>as:
      <ekz>
        ili dependas de la uzino, dependas de <tld/>antoj
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi submeti&gcirc;os al treege seriozaj riskoj, miaj <tld/>antoj estas tre postulemaj
        kaj ne ridas pri tiaj aferoj
        <fnt>
          <bib>ChV</bib>
          <lok>8</lok>
        </fnt>;
      </ekz>
      <ekz>
        EU <klr>[&#8230;]</klr> estas tutmonde la plej granda <tld/>anto por interpretistoj
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2004/008309.html">Plurlingvismo valoras siajn kostojn</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>antoj nun kompensas biciklan transporton de la lo&gcirc;ejo al la laborejo per
        po 0,15 e&ubreve;roj kilometre
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Monato;/2004/008262.html">Kiel solvi trafikproblemojn</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="labor.0donanto">labordonanto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_c;&c_a;&c_d;&c_a;&c_w;&c_c;&c_a;</trd>
  <trd lng="bg">&c_r;&c_a;&c_b;&c_o;&c_t;&c_o;&c_d;&c_a;&c_t;&c_je;&c_l;</trd>
  <trd
  lng="bo">&t_la;&t_sa;&t_tsheg;&t_ka;&t_tsheg;&t_sa;&t_pata;&t_rata;&t_naro;&t_da;&t_tsheg;&t_ma;&t_kha;&t_na;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>naj&iacute;matel</trd>,
    <trd>zam&ecaron;stnavatel</trd>
  </trdgrp>
  <trd lng="de">Arbeitgeber</trd>
  <trdgrp lng="es">
    <trd>empleador</trd>
  </trdgrp>
  <trd lng="en">employer</trd>
  <trdgrp lng="fr">
    <trd>employeur</trd>
    <trd>patron</trd>
  </trdgrp>
  <trd lng="fy">wurkjouwer</trd>
  <trd lng="nl">werkgever</trd>
  <trdgrp lng="ja">
    <trd>&#38599;&#12356;&#20027;  <pr>&#12420;&#12392;&#12356;&#12396;&#12375;</pr></trd>,
    <trd>&#38599;&#29992;&#32773;  <pr>&#12371;&#12424;&#12358;&#12375;&#12419;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pracodawca</trd>,
    <trd>najemca</trd>
  </trdgrp>
  <trd lng="pt">empregador</trd>
  <trd lng="sk">zamestn&aacute;vate&lcaron;</trd>
  <trd lng="uk">&c_n;&c_a;&c_j;&c_m;&c_a;&c_ch;</trd>
  <trd lng="zh">&#38599;&#20027; <pr>g&ugrave;zh&#468;</pr></trd>
