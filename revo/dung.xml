<drv mrk="dung.0ito">
  <kap><tld/>ito</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Iu <tld/>ita kaj laboranta kadre de tiu interkonsento:
      <ekz>
        kiel sklavo, kiu sopiras al ombro,
        kaj kiel <tld/>ito, kiu atendas sian pagon,
        tiel mi ricevis sorte monatojn vantajn
        <fnt><bib>MT</bib>, <lok>&Ijo; 7:2</lok></fnt>;
      </ekz>
      <ekz>
        la pago por <tld/>ito ne noktorestu &ccirc;e vi &gcirc;is mateno
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 19:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        laborante kiel <tld/>ito oni nur duone sentas sin kiel vera homo, li diris
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu el tiuj <tld/>itoj estas tiel forta, ke li povas egali ne du, sed kvar homojn
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj kunlaborantoj estas unu <tld/>ito kaj du nepagataj sekretariinoj
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la familio transposedigis la kafejojn al la <tld/>itoj, kiuj provis funkciigi
        ilin kiel laboristan kooperativon
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2005/008487.php">Fina fakturo</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_p;&c_r;&c_a;&c_c;&c_o;&c_w;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="bg">
    <trd>&c_s;&c_l;&c_u;&c_zh;&c_i;&c_t;&c_je;&c_l;</trd>,
    <trd>&c_r;&c_a;&c_b;&c_o;&c_t;&c_n;&c_i;&c_k;</trd>,
  </trdgrp>
  <trd lng="cs">zam&ecaron;stnanec</trd>
  <trdgrp lng="de">
    <trd>Angestellter</trd>,
    <trd>Besch&auml;ftigter</trd>
  </trdgrp>
  <trd lng="en">employee</trd>
  <trd lng="es">empleado</trd>
  <trdgrp lng="fr">
    <trd>employ&eacute;<klr tip="ind"> (subst.)</klr></trd>
    <trd>embauch&eacute;<klr tip="ind"> (subst.)</klr></trd>
    <trd>membre du <ind>personnel</ind></trd>
  </trdgrp>
  <trd lng="fy">wurknimmer</trd>
  <trd lng="nl">werknemer</trd>
  <trdgrp lng="ja">
    <trd>&#20351;&#29992;&#20154;  <pr>&#12375;&#12424;&#12358;&#12395;&#12435;</pr></trd>,
    <trd>&#34987;&#38599;&#29992;&#32773;  <pr>&#12402;&#12371;&#12424;&#12358;&#12375;&#12419;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pracownik</trd>,
    <trd>najmita</trd>,
    <trd>zatrudniony</trd>
  </trdgrp>
  <trd lng="pt">empregado</trd>
  <trdgrp lng="sk">
    <trd>zamestnanec</trd>,
    <trd>zriadenec</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_a;&c_j;&c_m;&c_a;&c_n;&c_i;&c_j;
    &c_r;&c_o;&c_b;&c_ib;&c_t;&c_n;&c_i;&c_k;/&c_s;&c_l;&c_u;&c_zh;&c_b;&c_o;&c_v;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_j;&c_m;&c_a;&c_n;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="zh">&#38599;&#21729; <pr>g&ugrave;yu&aacute;n</pr></trd>
</drv>

