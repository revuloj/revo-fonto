<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dung.xml,v 1.25 2021/05/29 14:32:19 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>dung</rad>/i
</kap>
<drv mrk="dung.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">JUR</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Interkonsenti kun laboristo, servisto a&ubreve; oficisto por
      havigi al si lian servadon la&ubreve; difinita salajro:
      <ekz>
        <tld/>i vartistinon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_j;&c_m;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_n;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_n;&c_a;&c_je;&c_m;&c_a;&c_m; &c_n;&c_a; &c_r;&c_a;&c_b;&c_o;&c_t;&c_a;</trd>
  <trd lng="bo">&#xF42;&#xFB1;&#xF62;&#xF0B;&#xF56;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>najmout do pr&aacute;ce</trd>,
    <trd>p&rcaron;ijmout do zam&ecaron;stn&aacute;n&iacute;</trd>,
    <trd>zam&ecaron;stnat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>einstellen</trd>,
    <trd>dingen</trd>,
    <trd>anheuern</trd>,
    <trd>in Dienst nehmen</trd>,
    <trd>in Lohn nehmen</trd>
  </trdgrp>
  <trd lng="en">hire</trd>
  <trd lng="es">contratar</trd>
  <trdgrp lng="fr">
    <trd>embaucher</trd>
    <trd>engager<klr tip="ind"> (embaucher)</klr></trd>
    <trd>prendre<klr tip="ind"> (embaucher)</klr></trd>
    <trd>recruter<klr tip="ind"> (embaucher)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>alkalmaz</trd>,
    <trd>felvesz</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zatrudnia&cacute;</trd>,
    <trd>najmowa&cacute;</trd>,
    <trd>przyjmowa&cacute;<klr> do pracy</klr></trd>,
    <trd>anga&zdot;owa&cacute;<klr> do pracy</klr><klr>
    (przest.)</klr></trd>,
    <trd>podpisa&cacute; umow&eogonek; o prac&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>empregar</trd>,
    <trd>contratar</trd>,
    <trd>assaliar</trd>,
    <trd>engajar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>naja&tcaron;</trd>,
    <trd>zamestna&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="dung.sin0igi">
  <kap>sin <tld/>igi</kap>
  <snc>
    <dif>
      Interkonsenti kun <tld/>anto.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_j;&c_m;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><klr>sich </klr>verdingen</trd>,
    <trd>anheuern</trd>,
    <trd>einstellen lassen</trd>,
    <trd>eingestellt werden</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se faire <ind>embaucher</ind></trd>
    <trd>se faire <ind>engager</ind></trd>
    <trd>se faire <ind>recruter</ind></trd>
  </trdgrp>
  <trd lng="hu">elhelyezkedk<klr tip="ind">
  (&aacute;ll&aacute;sban)</klr></trd>
  <trd lng="ru">&c_n;&c_a;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="dung.0anto">
  <kap><tld/>anto</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Iu, kiu <tld/>as.
    </dif>
    <refgrp tip="vid">
      <ref cel="labor.0donanto">labordonanto</ref>
    </refgrp>
  </snc>
  <trd lng="bg">&c_r;&c_a;&c_b;&c_o;&c_t;&c_o;&c_d;&c_a;&c_t;&c_je;&c_l;</trd>
  <trd lng="bo">&#xF63;&#xF66;&#xF0B;&#xF40;&#xF0B;&#xF66;&#xFA4;&#xFB2;&#xF7C;&#xF51;&#xF0B;&#xF58;&#xF41;&#xF53;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>naj&iacute;matel</trd>,
    <trd>zam&ecaron;stnavatel</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>empleador</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>employeur</trd>
    <trd>patron</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pracodawca</trd>,
    <trd>najemca</trd>
  </trdgrp>
  <trd lng="sk">zamestn&aacute;vate&lcaron;</trd>
</drv>

<drv mrk="dung.0ito">
  <kap><tld/>ito</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Iu <tld/>ita kaj laboranta kadre de tiu interkonsento:
      <ekz>
        kiel sklavo, kiu sopiras al ombro,
        kaj kiel <tld/>ito, kiu atendas sian pagon,
        tiel mi ricevis sorte monatojn vantajn
        <fnt><bib>MT</bib>, <lok>&Ijo; 7:2</lok></fnt>;
      </ekz>
      <ekz>
        asekuri siajn <tld/>itojn kontra&ubreve; akcidentoj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_s;&c_l;&c_u;&c_zh;&c_i;&c_t;&c_je;&c_l;</trd>,
    <trd>&c_r;&c_a;&c_b;&c_o;&c_t;&c_n;&c_i;&c_k;</trd>,
  </trdgrp>
  <trd lng="cs">zam&ecaron;stnanec</trd>
  <trdgrp lng="de">
    <trd>Angestellter</trd>
  </trdgrp>
  <trd lng="en">employee</trd>
  <trd lng="es">empleado</trd>
  <trdgrp lng="fr">
    <trd>employ&eacute;<klr tip="ind"> (subst.)</klr></trd>
    <trd>embauch&eacute;<klr tip="ind"> (subst.)</klr></trd>
    <trd>membre du <ind>personnel</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pracownik</trd>,
    <trd>najmita</trd>,
    <trd>zatrudniony</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>zamestnanec</trd>,
    <trd>zriadenec</trd>
  </trdgrp>
</drv>

<drv mrk="dung.0itaro">
  <kap><tld/>itaro</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Aro de &ccirc;iuj <tld/>itoj:
      <ekz>
        kiam deputito efektive volas uzi sian propran lingvon, ofte
        necesas
        improvize ser&ccirc;i hazardan angligonton inter la parlamenteja
        <tld/>itaro
        <fnt>
        <aut>Istv&aacute;n Ertl</aut>
        <vrk>La angla efikas naci-unuige</vrk>
        <bib>LOdE</bib>
        <lok>2003:3 (101)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="person.0aro.EKON">personaro</ref>,
      <ref cel="etat.0o.personaro">etato <sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="bg">&c_p;&c_je;&c_r;&c_s;&c_o;&c_n;&c_a;&c_l;</trd>
  <trd lng="cs">zam&ecaron;stnanci</trd>
  <trdgrp lng="fr">
    <trd>personnel<klr tip="ind"> (ensemble des
    employ&eacute;s)</klr></trd>
    <trd>effectif<klr tip="ind"> (ensemble des
    employ&eacute;s)</klr></trd>
    <trd>employ&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pracownicy</trd>
  </trdgrp>
  <trd lng="sk">zamestnanci</trd>
</drv>

<drv mrk="dung.0iteco">
  <kap><tld/>iteco</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Stato de <tld/>ito:
      <ekz>
        Vina&rcaron; &scirc;ajne maldistingas sklavecon disde
        <tld/>iteco
        <fnt>
        <aut>Jenovk Lazian</aut>
        <vrk>Vandale pri Jozefo</vrk>
        <bib>LOdE</bib>
        <lok>2004:2 (112)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">pracovn&iacute; pom&ecaron;r</trd>
  <trdgrp lng="fr">
    <trd>travail<klr tip="ind"> (l'&eacute;tat
    d'employ&eacute;)</klr></trd>
    <trd>emploi<klr tip="ind"> (l'&eacute;tat
    d'employ&eacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zatrudnienie</trd>,
    <trd>anga&zdot;</trd>,
    <trd>najem</trd>
  </trdgrp>
  <trd lng="sk">pracovn&yacute; pomer</trd>
</drv>

<drv mrk="dung.mal0i">
  <kap>mal<tld/>i</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">JUR</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <klr>(iun)</klr>
      &Ccirc;esigi ties <tld/>itecon:
      <ekz>
        mi mal<tld/>is lin pro malhonesteco.
      </ekz>
    </dif>
    <refgrp tip="super">
      <ref cel="eks.0igi.iun">eksigi <sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="bg">&c_u;&c_v;&c_o;&c_l;&c_n;&c_ja;&c_v;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>d&aacute;t v&yacute;pov&ecaron;&dcaron;</trd>,
    <trd>propustit ze zam&ecaron;stn&aacute;n&iacute;</trd>,
    <trd>rozv&aacute;zat pracovn&iacute; pom&ecaron;r</trd>,
    <trd>vypov&ecaron;d&ecaron;t z pracovn&iacute;ho pom&ecaron;ru</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cong&eacute;dier<klr tip="ind"> (licencier)</klr></trd>
    <trd>chasser<klr tip="ind"> (licencier)</klr></trd>
    <trd><ind>d&eacute;gager</ind> de ses obligations</trd>
    <trd><ind>d&eacute;mettre</ind> de ses fonctions</trd>
    <trd>d&eacute;missionner<klr tip="ind"> (tr,
    d&eacute;mettre)</klr></trd>
    <trd>licencier<klr tip="ind"> (limoger)</klr></trd>
    <trd>limoger</trd>
    <trd><ind>radier</ind> des effectifs</trd>
    <trd><ind>rayer</ind> de dessus l'&eacute;tat<klr tip="ind"> (vx,
    limoger)</klr></trd>
    <trd><ind>rayer</ind> des cadres</trd>
    <trd><ind>rayer</ind> des effectifs</trd>
    <trd><ind>relever</ind> de ses fonctions</trd>
    <trd>remercier<klr tip="ind"> (licencier)</klr></trd>
    <trd>renvoyer<klr tip="ind"> (licencier)</klr></trd>
    <trd>r&eacute;voquer<klr tip="ind"> (limoger)</klr></trd>
    <trd>mettre &agrave; la <ind>porte</ind><klr tip="ind">
    (licencier)</klr></trd>
    <trd>lourder<klr tip="ind"> (licencier)</klr></trd>
    <trd>vider<klr tip="ind"> (fam., licencier)</klr></trd>
    <trd>virer<klr tip="ind"> (fam., licencier)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zwolni&cacute;<klr> z pracy</klr></trd>,
    <trd>wym&oacute;wi&cacute;<klr> prac&eogonek;</klr></trd>,
    <trd>rozwi&aogonek;za&cacute; umow&eogonek; o prac&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>prepusti&tcaron; <klr>(z pr&aacute;ce, zo
    slu&zcaron;ieb)</klr></trd>,
    <trd>prepusti&tcaron; <klr>(z pr&aacute;ce| zo
    slu&zcaron;ieb)</klr></trd>
  </trdgrp>
</drv>
</art>
