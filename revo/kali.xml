<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kali.xml,v 1.29 2023/12/02 21:20:04 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>kali</rad>/o
</kap>
<drv mrk="kali.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">K</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="19">elemento</ref>
      kun atomnumero 19, atompezo 39,102;
      metala, vapori&gcirc;ema; oksidi&gcirc;anta en malseka aero:
      <ekz>
        oni uzas la specialajn salajn solva&jcirc;ojn, entenantajn en necesaj koncentritecoj
        jonojn de natrio, <tld/>o, kloro kaj aliaj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2010/010071.php">Antikva, sed ankora&ubreve; malica gasto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        granito radioaktivas pro la izotopoj de <tld/>o, torio kaj uranio
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2013/011075.php">Poeto dan&gcirc;era</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        oni utiligis atomojn de <tld/>o-39, malvarmigitajn pere de lasera lumo
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011214.php">2013: dek donacoj al la posteuloj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="santiag.0o_de_Kalio">Kalio (kolombia urbo)</ref>
  <trd lng="be">&c_k;&c_a;&c_l;&c_ib;&c_j;</trd>
  <trd lng="bg">&c_k;&c_a;&c_l;&c_i;&c_j;</trd>
  <trd lng="br">potasiom</trd>
  <trd lng="ca">potassi</trd>
  <trdgrp lng="cs">
    <trd>drasl&iacute;k</trd>,
    <trd>kalium</trd>
  </trdgrp>
  <trd lng="de">Kalium</trd>
  <trd lng="en">potassium</trd>
  <trd lng="es">potasio</trd>
  <trd lng="fr">potassium</trd>
  <trd lng="hu">k&aacute;lium</trd>
  <trd lng="id">kalium</trd>
  <trd lng="it">potassio</trd>
  <trd lng="ja">&#x30AB;&#x30EA;&#x30A6;&#x30E0;</trd>
  <trd lng="la">Kalium</trd>
  <trd lng="nl">kalium</trd>
  <trd lng="pl">potas</trd>
  <trd lng="pt">pot&aacute;ssio</trd>
  <trd lng="ru">&c_k;&c_a;&c_l;&c_i;&c_j;</trd>
  <trd lng="sk">drasl&iacute;k</trd>
  <trd lng="sv">kalium</trd>
  <trd lng="uk">&#x43A;&#x430;&#x43B;&#x456;&#x439;</trd>
  <trd lng="zh">&#x9240; <pr>ji&#x1CE;</pr></trd>
</drv>
</art>

