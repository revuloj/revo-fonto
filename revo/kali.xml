<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>kali</rad>/o
</kap>
<drv mrk="kali.0o">
  <kap><tld/>o</kap>
  <mlg>K</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref>
      kun atomnumero 19, atompezo 39,102, simbolo K.
