<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>kali</rad>/o
</kap>
<drv mrk="kali.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">K</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
      lst="voko:&hcirc;emiaj_elementoj" val="19">elemento</ref>
      kun atomnumero 19, atompezo 39,102;
      metala, vapori&gcirc;ema; oksidi&gcirc;anta en malseka aero.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_ib;&c_j;</trd>  
  <trd lng="bg">&c_k;&c_a;&c_l;&c_i;&c_j;</trd>   
  <trd lng="br">potasiom</trd>
  <trd lng="ca">potassi</trd>
  <trd lng="de">Kalium</trd>
  <trd lng="en">potassium</trd>
  <trd lng="es">potasio</trd>
  <trd lng="fr">potassium</trd>
  <trd lng="hu">k&aacute;lium</trd>
  <trd lng="it">potassio</trd>
  <trd lng="la">Kalium</trd>
  <trd lng="nl">kalium</trd>
  <trd lng="pl">potas</trd>
  <trd lng="pt">pot&aacute;ssio</trd>
  <trd lng="ru">&c_k;&c_a;&c_l;&c_i;&c_j;</trd>
  <trd lng="sv">kalium</trd>
</drv>
</art>

