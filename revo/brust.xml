<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: brust.xml,v 1.34 2023/10/29 19:11:03 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>brust</rad>/o
</kap>
<drv mrk="brust.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Parto de la korpo, ekde kolo &gcirc;is ventro, enhavanta la pulmojn kaj la
      koron:
      <ekz>
        <nom>Naomi</nom> prenis la infanon kaj metis &gcirc;in sur sian <tld/>on kaj fari&gcirc;is &gcirc;ia
        vartistino
        <fnt>
          <bib>MT</bib>
          <lok>Rut 4:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        koleremeco lo&gcirc;as en la <tld/>o de malsa&gcirc;uloj
        <fnt>
          <bib>MT</bib>
          <lok>Predikanto 7:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        li traboras al ili la <tld/>on per sia sago
        <fnt>
          <bib>Fab1</bib>
          <lok>Malbonkonduta knabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia kapo ripozis sur &scirc;ia <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        en leda saketo sur la <tld/>o li havis kelke da alkudritaj oraj moneroj
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        melankolio <klr>[&#x2026;]</klr> plenigis lian <tld/>on
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        a&ubreve;di&gcirc;is la koraj kaj pensaj sonoj el la <tld/>o de la homoj
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        vivi kiel &ccirc;e la <tld/>o de Dio
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        dorloti serpenton sur sia <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bust.0o">busto</ref>,
      <ref cel="mam.0o">mamo</ref>,
      <ref cel="sin.0o">sino</ref>,
      <ref cel="tali.0o">talio</ref>,
      <ref cel="tors.0o">torso</ref>
    </refgrp>
  </snc>
  <snc mrk="brust.0o.mamo">
    <dif>
      <ref tip="dif" cel="mam.0o">Mamo</ref>:
      <ekz>
        sub la diafana &scirc;tofo oni vidis &scirc;iajn virgajn <tld/>ojn, similajn al pomoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        se iu <klr>[&#x2026;]</klr> ne elsu&ccirc;os el &scirc;ia dekstra <tld/>o tri gutojn da sango kaj
        ne elkra&ccirc;os ilin senprokraste, la princino vere mortos
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fidela Johano</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x80F8;&#x812F; <pr>xi&#x14D;ngp&#xFA;</pr></trd>
</snc>
  <trd lng="be">&c_g;&c_r;&c_u;&c_d;&c_z;&c_ib;</trd>
  <trd lng="bo">&t_na;&t_shabchu;&t_tsheg;&t_ma;&t_tsheg;</trd>
  <trd lng="br">bruched</trd>
  <trdgrp lng="cs">
    <trd>hru&dcaron;</trd>,
    <trd>poprs&iacute;</trd>,
    <trd>prsa</trd>,
    <trd>&ncaron;adra</trd>
  </trdgrp>
  <trd lng="de">Brust</trd>
  <trdgrp lng="en">
    <trd>chest<klr tip="ind"> (body part)</klr></trd>,
    <trd>breast<klr tip="ind"> (chest)</klr></trd>
  </trdgrp>
  <trd lng="es">pecho</trd>
  <trd lng="fr">poitrine</trd>
  <trd lng="he">&het;&zayin;&he;</trd>
  <trd lng="hu">mell</trd>
  <trd lng="id">dada</trd>
  <trd lng="it">petto<klr> (anat.)</klr></trd>
  <trd lng="ja">&#x80F8;  <pr>&#x3080;&#x306D;</pr></trd>
  <trd lng="nl">borst<klr tip="ind"> (borstkas)</klr></trd>
  <trd lng="pl">pier&sacute;</trd>
  <trd lng="pt">peito</trd>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_d;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>hru&dcaron;</trd>,
    <trd>hru&dcaron;</trd>,
    <trd>poprsie</trd>,
    <trd>prsia</trd>,
    <trd>&ncaron;adr&aacute;</trd>
  </trdgrp>
  <trd lng="tr">g&ouml;&gbreve;&uuml;s</trd>
  <trd lng="uk">&c_g;&c_r;&c_u;&c_d;&c_i;</trd>
</drv>

<drv mrk="brust.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Viando de <tld/>o:
      <ekz>
        Moseo prenis la <tld/>a&jcirc;on kaj skuis &gcirc;in kiel skuoferon
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 8:29</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_u;&c_d;&c_z;&c_ib;&c_n;&c_k;&c_a;</trd>    
  <trd lng="br">bruched<klr tip="ind"> (boued)</klr></trd>
  <trd lng="de">Brustst&uuml;ck</trd>
  <trd lng="es">pechuga</trd>
  <trd lng="fr">poitrine<klr tip="ind"> (viande)</klr></trd>
  <trd lng="he">&het;&zayin;&he;<klr tip="ind"> (&bet;&shin;&resh;)</klr></trd>
  <trdgrp lng="hu">
    <trd>melle h&uacute;sa</trd>,
    <trd>szegy</trd>
  </trdgrp>
  <trd lng="it">petto<klr> (alim.)</klr></trd>
  <trd lng="ja">&#x80F8;&#x8089;  <pr>&#x3080;&#x306D;&#x306B;&#x304F;</pr></trd>
  <trd lng="pl">pier&sacute;</trd>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_d;&c_i;&c_n;&c_k;&c_a;</trd>
  <trd lng="uk">&c_g;&c_r;&c_u;&c_d;&c_i;&c_n;&c_k;&c_a;&nbsp;(&c_m;&#x2019;&c_ja;&c_s;&c_o;)</trd>
</drv>

<drv mrk="brust.cxe0ocxe0ajxo">
  <kap>&ccirc;e<tld/>a&jcirc;o, <var><kap>&ccirc;e<tld/>o</kap></var></kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Parto de vesto, situanta &ccirc;e la <tld/>o:
      <ekz>
        Levio ser&ccirc;is en la &ccirc;e<tld/>o kaj eligis pergamenrulon
        <fnt><bib>MkM</bib><lok>&ccirc;ap. 26</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i estis vestita per vesto tre simila al tiu, kiun portis
        la Betra&hcirc;anino, krom tio, ke &gcirc;i ne havis broditan
        kolombon sur la &ccirc;e<tld/>a&jcirc;o
        <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 10</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="korsaj.0o">korsa&jcirc;o</ref>,
      <ref cel="zon.mam0o">mamzono</ref>
    </refgrp>
  </snc>                             
  <trd lng="be">&c_p;&c_a;&c_z;&c_u;&c_h;&c_a;</trd>
  <trd lng="br">bruched<klr tip="ind"> (dilhad)</klr></trd>
  <trd lng="de">Mieder</trd>
  <trd lng="fr">poitrine<klr tip="ind"> (v&ecirc;tement)</klr></trd>
  <trd lng="it">petto<klr> (abigl.)</klr></trd>
  <trd lng="uk">&c_p;&c_a;&c_z;&c_u;&c_h;&c_a;</trd>
</drv>

<drv mrk="brust.sur0ajxo">
  <kap>
    sur<tld/>a&jcirc;o, <var><kap>sur<tld/>o</kap></var>
  </kap>
  <snc mrk="brust.sur0ajxo.KOMUNE">
    <dif>
      Parto de vesto kovranta la bruston; 
      <ref tip="sin" cel="brust.cxe0ocxe0ajxo">&ccirc;ebrusta&jcirc;o</ref>:
      <ekz>
        &scirc;i elprenis la leteron el sia sur<tld/>o
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        malestas la zono, mi do kisu la sur<tld/>a&jcirc;on
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Mieder</trd>
    <trd lng="zh">&#x7D27;&#x8EAB;&#x56F4;&#x8170; <pr>j&#x1D0;nsh&#x113;nw&#xE9;iy&#x101;o</pr></trd>
</snc>
  <snc>
    <uzo tip="fak">BIB</uzo>
    <dif>
      <tld lit="B"/>po&scirc;o, kiu apartenis al la <ref tip="malprt" cel="ornat.0o">ornato</ref> 
      de juda &ccirc;efpastro:
      <ekz>
        alligu la sur<tld/>a&jcirc;on per &gcirc;iaj ringoj al la ringoj de la efodo per la&ccirc;o el
        blua &scirc;tofo, por ke &gcirc;i estu super la zono de la efodo
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 28:28</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Choschen</trd>
    <trd lng="en">breastplate</trd>
    <trd lng="fr">pectoral</trd>
    <trd lng="ru">&c_n;&c_a;&c_p;&c_je;&c_r;&c_s;&c_n;&c_i;&c_k;</trd>
  </snc>
  <trd lng="ja">&#x80F8;&#x98FE;&#x308A;  <pr>&#x3080;&#x306D;&#x304B;&#x3056;&#x308A;</pr></trd>
</drv>




</art>
