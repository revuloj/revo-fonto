<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>uvertur</rad>/o
</kap>
<drv mrk="uvertu.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="konduk.en0o">Enkonduko</ref> de orkestra
      <ref tip="malprt" cel="kompon.0ajxo">kompona&jcirc;o</ref>,
      ekzerca por la muzikantoj kaj anonca pri la stilo de la tuto,
      eventuale memstare ludebla.
    </dif>
    <refgrp tip="vid">
      <ref cel="prelud.0o">preludo</ref>
    </refgrp>
  </snc>
  <trd lng="de">Ouvert&uuml;re</trd>
  <trd lng="es">obertura</trd>
  <trdgrp lng="fr">
    <trd>ouverture<klr> (musicale)</klr></trd>
  </trdgrp>
  <trd lng="nl">ouverture</trd>
  <trd lng="pt">abertura</trd>
  <trd lng="ru">&c_u;&c_v;&c_je;&c_r;&c_t;&c_ju;&c_r;&c_a;</trd>
</drv>
</art>
