  <trd lng="is">Fenn&oacute;skand&iacute;a</trd>
  <trd lng="it">Fennoscandia</trd>
  <trd lng="lt">Fenoskandija</trd>
  <trd lng="nl">Fennoscandi&euml;</trd>
  <trd lng="no">Fennoskandia</trd>
