<vortaro>
<art mrk="$Id: retros.xml,v 1.15 2022/10/31 21:31:33 revo Exp $">
<kap>
  <rad>retrospektiv</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="retros.0o">
  <kap><tld/>o</kap>
  <snc mrk="retros.0o.KOMUNE">
    <dif>
      Konsidero de pasinta tempo, de
      pasintaj agadoj:
      <ekz>
        <tld/>oj estas gravaj por &ccirc;iuj ni,
        precipe por la &leftquot;preska&ubreve; maljunuloj&rightquot;
        <fnt>
          <aut>O. Milhomens</aut>,
          <vrk><url ref="http://www.ipernity.com/doc/79333/6228118/comment/7494068#comment7494068">
          Retrospektivo</url></vrk>,
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="perspe.0o.FIG">perspektivo<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>pohled do minulosti</trd>,
    <trd>retrospektiva</trd>
  </trdgrp>
  <trd lng="fr">r&eacute;trospective<klr> (subst.)</klr></trd>
  <trd lng="pl">retrospekcja</trd>
  <trd lng="ro">flashback</trd>
  <trd lng="sk">poh&#x13E;ad do minulosti</trd>
</drv>

<drv mrk="retros.0a">
  <kap><tld/>a</kap>
  <snc mrk="retros.0a.KOMUNE">
    <dif>
      Prezentanta pasintan tempon, retrorigarda.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_t;&c_r;&c_a;&c_s;&c_p;&c_e;&c_k;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>retrospektivn&#xED;</trd>,
    <trd>zam&#x11B;&rcaron;en&#xFD; na minulost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>retrospektiv</trd>,
    <trd>r&uuml;ckschauend</trd>,
    <trd>r&uuml;ckblickend</trd>
  </trdgrp>
  <trd lng="en">retrospective</trd>
  <trd lng="fr">r&eacute;trospectif</trd>
  <trdgrp lng="hu">
    <trd>retrospekt&iacute;v</trd>,
    <trd>visszatekint&odblac;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>terugziend</trd>,
    <trd>retrospectief</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>retrospektywny</trd>,
    <trd>spogl&aogonek;daj&aogonek;cy wstecz</trd>
  </trdgrp>
  <trd lng="pt">retrospectivo</trd>
  <trdgrp lng="ro">
    <trd>retrospectiv</trd>,
    <trd>privind in urma</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_t;&c_r;&c_o;&c_s;&c_p;&c_je;&c_k;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">retrospekt&#xED;vny</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_je;&c_t;&c_r;&#x43E;&c_s;&#x43F;&c_je;&#x43A;&c_t;&c_i;&c_v;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_v;&c_je;&c_r;&#x43D;&c_je;&#x43D;&c_i;&c_j; &c_d;&#x43E; &#x43C;&c_i;&#x43D;&c_u;&#x43B;&#x43E;&c_g;&#x43E;</trd>
  </trdgrp>
</drv>
</art>
