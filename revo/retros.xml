<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: retros.xml,v 1.21 2024/04/13 20:15:53 revo Exp $">
<kap>
  <rad>retrospektiv</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="retros.0o">
  <kap><tld/>o</kap>
  <snc mrk="retros.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="konsid.0o">Konsidero</ref> de pasinta tempo, de pasintaj agadoj:
      <ekz>
        <tld/>oj estas gravaj por &ccirc;iuj ni,
        precipe por la &leftquot;preska&ubreve; maljunuloj&rightquot;
        <fnt>
          <aut>O. Milhomens</aut>,
          <vrk><url ref="http://www.ipernity.com/doc/79333/6228118/comment/7494068#comment7494068">
          Retrospektivo</url></vrk>,
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Minnaja</nom> prezentis <tld/>on ne nur al la historio de <nom>AIS</nom>, sed anka&ubreve; al
        pli fruaj
        sciencaj aktivadoj
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 10 (108)</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Pennacchietti</nom> <tld/>is kaj perspektivis pri la universitata instruado de
        interlingvistiko
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#x2116;3</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="perspe.0o.FIG">perspektivo<sncref/></ref>,
      <ref cel="memor.re0o">rememoro</ref>,
      <ref cel="resum.0o">resumo</ref>,
      <ref cel="revizi.0o.ekzamenado">revizio<sncref/></ref>
    </refgrp>
  </snc>
<trd lng="be">&c_r;&c_e;&c_t;&c_r;&c_a;&c_s;&c_p;&c_e;&c_k;&c_t;&c_y;&c_v;&c_a;</trd>
<trd lng="en">retrospective</trd>
  <trdgrp lng="cs">
    <trd>pohled do minulosti</trd>,
    <trd>retrospektiva</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>R&uuml;ckblick</trd>,
    <trd>R&uuml;ckschau</trd>,
    <trd>Retrospektive</trd>
  </trdgrp>
  <trd lng="fr">r&eacute;trospective<klr> (subst.)</klr></trd>
  <trd lng="pl">retrospekcja</trd>
  <trd lng="ro">flashback</trd>
  <trd lng="sk">poh&lcaron;ad do minulosti</trd>
  <trdgrp lng="zh">
    <trd>&#x56DE;&#x9867;&#x5C55; <pr>hu&#xED;g&#xF9;zh&#x1CE;n</pr></trd>,
    <trd>&#x56DE;&#x987E;&#x5C55; <pr>hu&#xED;g&#xF9;zh&#x1CE;n</pr></trd>,
    <trd>&#x56DE;&#x9867; <pr>hu&#xED;g&#xF9;</pr></trd>,
    <trd>&#x56DE;&#x987E; <pr>hu&#xED;g&#xF9;</pr></trd>,
    <trd>&#x56DE;&#x60F3; <pr>hu&#xED;xi&#x1CE;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="retros.0a">
  <kap><tld/>a</kap>
  <snc mrk="retros.0a.KOMUNE">
    <dif>
      Prezentanta, konsideranta pasintan tempon, retrorigarda:
      <ekz>
        <tld/>a rigardo al 50 jaroj da apliko de Esperanto en la scienco
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        en interna <tld/>a raporto <klr>[ili notis]</klr>, ke estis tute erara opinio <klr>[&#x2026;]</klr>
        <fnt>
          <bib>EeP</bib>
          <lok>Kvina Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e pensante pri faligita domo de la infana&gcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2008/009298.php">Orient-azia itinero</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a ekspozicio de grafika&jcirc;oj kaj miniaturaj libroj
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 7 (69)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kronik1.0a">kronika</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_t;&c_r;&c_a;&c_s;&c_p;&c_e;&c_k;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>retrospektivn&iacute;</trd>,
    <trd>zam&ecaron;&rcaron;en&yacute; na minulost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>r&uuml;ckblickend</trd>,
    <trd>zur&uuml;ckblickend</trd>,
    <trd>r&uuml;ckschauend</trd>,
    <trd>retrospektiv</trd>
  </trdgrp>
  <trd lng="en">retrospective</trd>
  <trd lng="fr">r&eacute;trospectif</trd>
  <trdgrp lng="hu">
    <trd>retrospekt&iacute;v</trd>,
    <trd>visszatekint&odblac;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x56DE;&#x9867;&#x7684;&#x306A;  <pr>&#x304B;&#x3044;&#x3053;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x904E;&#x53BB;&#x3092;&#x632F;&#x308A;&#x8FD4;&#x308B;  <pr>&#x304B;&#x3053;&#x3092;&#x3075;&#x308A;&#x304B;&#x3048;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>terugziend</trd>,
    <trd>retrospectief</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>retrospektywny</trd>,
    <trd>spogl&aogonek;daj&aogonek;cy wstecz</trd>
  </trdgrp>
  <trd lng="pt">retrospectivo</trd>
  <trdgrp lng="ro">
    <trd>retrospectiv</trd>,
    <trd>privind in urma</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_t;&c_r;&c_o;&c_s;&c_p;&c_je;&c_k;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">retrospekt&iacute;vny</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_je;&c_t;&c_r;&c_o;&c_s;&c_p;&c_je;&c_k;&c_t;&c_i;&c_v;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_v;&c_je;&c_r;&c_n;&c_je;&c_n;&c_i;&c_j; &c_d;&c_o; &c_m;&c_i;&c_n;&c_u;&c_l;&c_o;&c_g;&c_o;</trd>
  </trdgrp>
  <trd lng="zh">&#x56DE;&#x7738; <pr>hu&#xED;m&#xF3;u</pr></trd>
</drv>
</art>
