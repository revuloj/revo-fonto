<drv mrk="kalabri.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="kalabri.0o.regiono">
    <dif>
      Plej suda <ref tip="lst" lst="voko:regionoj" cel="region.0o.land">regiono</ref>
      de la <ref tip="malprt" cel="ital.I0ujo">itala</ref> duoninsulo, 
      figure t.e. la pinto de la &leftquot;boto&rightquot;:
      <ekz>
        en la premiita romano familio el <tld/>o frontas al diversaj sufera&jcirc;oj, sed
        fine, pro sia ekzemplo, esperigas aliajn homojn
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2013/010854.php">Premion enblovas vento</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        terura <klr>[tertremo en 1908]</klr> trafis Sicilion kaj <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2013/010777.php">&Gcirc;is la tero tremas</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_K;&c_a;&c_l;&c_ja;&c_b;&c_r;&c_y;&c_ja;</trd>
    <trd lng="de">Kalabrien</trd>
    <trd lng="en">Calabria</trd>
    <trd lng="fr">Calabre</trd>
  </snc>
  <trd lng="ja">&#x30AB;&#x30E9;&#x30D6;&#x30EA;&#x30A2;&#x5730;&#x65B9;  [<ind>&#x30AB;&#x30E9;&#x30D6;&#x30EA;&#x30A2;&#x3061;&#x307B;&#x3046;</ind>]</trd>
  <trd lng="uk">&#x41A;&#x430;&#x43B;&#x430;&#x431;&#x440;&#x456;&#x44F;</trd>
</drv>

