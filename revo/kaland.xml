<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kalandr</rad>/o
</kap>
<drv mrk="kaland.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <fnt><bib>PV</bib></fnt>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="kurkul1.0oj">kurkuliedoj</ref>
      (<trd lng="la">Sitophilus</trd>), kies larvo kreskas ene de grengrajnoj.
    </dif>
    <refgrp tip="sin">
      <ref cel="skarab.gren0o">grenskarabo</ref>,
      <ref cel="kaland2.0o">kalandrao</ref>.
    </refgrp>
  </snc>
  <refgrp tip="hom">
    Ne miksu kun:
    <ref cel="kaland3.0o">kalandro</ref> (birdo),
    <ref cel="kaland1.0o">kalandro</ref> (ago kalandri).
  </refgrp>
  <trd lng="de">Kornk&auml;fer</trd>
  <trd lng="en">corn weevil</trd>
  <trd lng="fr">calandre<klr tip="ind"> (charan&ccedil;on)</klr></trd>
  <trd lng="hu">zsizsik</trd>
  <trd lng="nl">kalander</trd>
  <trd lng="ru">&c_h;&c_l;&c_je;&c_b;&c_n;&c_y;&c_j; &c_d;&c_o;&c_l;&c_g;&c_o;&c_n;&c_o;&c_s;&c_i;&c_k;</trd>
</drv>
</art>
