<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kaland.xml,v 1.24 2025/04/06 18:00:23 revo Exp $">
<kap>
  <rad>kalandr</rad>/o <fnt><bib>PVEF</bib></fnt>,
  <var><kap><rad>kalandra</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>
<drv mrk="kaland.0o">
  <kap><tld/>o,
    <var><kap>kalandrao</kap></var>
  </kap>
  <snc mrk="kaland.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="skarab.gren0o.ZOO">Grenskarabo</ref>:
      <ekz>
        kurkuliedoj estas familio de koleopteroj al kiu apartenas <tld/>ao kaj kurkulio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=335673">Kurkuliedoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="kaland.0o.RIM_HOM">
    Ne miksu kun:
    <ref tip="hom" cel="kaland3.0o">kalandro</ref> (birdo),
    <ref tip="hom" cel="kaland1.0o">kalandro</ref> (ago kalandri).
  </rim>
  <trdgrp lng="be">
    <trd>&c_s;&c_l;&c_o;&c_n;&c_ib;&c_k; <klr>(&c_zh;&c_u;&c_k;)</klr></trd>,
    <trd>&c_sh;&c_a;&c_sh;&c_o;&c_l;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">corc del blat</trd>
  <trd lng="de">Kornk&auml;fer</trd>
  <trd lng="en">corn weevil</trd>
  <trdgrp lng="fi">
    <trd>jyv&auml;k&auml;rs&auml;k&auml;s</trd>,
    <trd>(Sitophilus-suvun) <ind>k&auml;rs&auml;k&auml;s</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>calandre<klr tip="ind"> (charan&ccedil;on)</klr></trd>,
    <trd>charan&ccedil;on</trd>
  </trdgrp>
  <trd lng="hu">zsizsik</trd>
  <trd lng="ja">&#12467;&#12463;&#12478;&#12454;&#12512;&#12471;</trd>
  <trd lng="nl">kalander</trd>
  <trd lng="pl">wo&lstroke;ek</trd>
  <trd lng="ru">&c_h;&c_l;&c_je;&c_b;&c_n;&c_y;&c_j;
  &c_d;&c_o;&c_l;&c_g;&c_o;&c_n;&c_o;&c_s;&c_i;&c_k;</trd>
  <trd lng="uk">&c_k;&c_o;&c_m;&c_ib;&c_r;&c_n;&c_i;&c_j; &c_d;&c_o;&c_v;&c_g;&c_o;&c_n;&c_o;&c_s;&c_i;&c_k;</trd>
  <trd lng="zh">&#34482; <pr>p&iacute;ng</pr></trd>
</drv>
</art>
