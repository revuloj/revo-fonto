<drv mrk="polur.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="polur.0i.KOMUNE">
    <dif>
      <ref cel="glat.0igi">Glatigi</ref> kaj <ref tip="super" cel="bril.0igi">briligi</ref>
      ion per frotado:
      <ekz>
        <tld/>i lignon, vitron, marmoron;
      </ekz>
      <ekz>
        ili facetis kaj <tld/>is multekostajn &scirc;tonojn
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili drinkas e&ccirc; <klr>[&#x2026;]</klr> e&ccirc; <ind><tld/>kremo</ind><klr>[n]</klr>, enhavanta alkoholon
        kaj uzata por <tld/>i latunon ktp
        <fnt>
          <bib>KrM</bib>
          <lok>31. Ricevis Bandito La&ubreve; Sia Merito.</lok>
        </fnt>;
        <trd lng="de">Politur</trd>
      </ekz>
      <ekz>
        la maljuna re&gcirc;o lasis <tld/>i sian oran kronon
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        granitaj platrokoj, kiujn <klr>[&#x2026;]</klr> rabotis kaj <tld/>is la giganta glacio
        <fnt>
          <bib>ViV</bib>
          <lok>IV.</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="facet.0i">faceti</ref>,
      <ref cel="glace.0igi">glaceigi</ref>,
      <ref cel="sxlif.0i">&scirc;lifi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>polieren</trd>,
      <trd>glatt und <ind>gl&auml;nzend</ind> machen</trd>,
      <trd>zum <ind>Gl&auml;nzen</ind> bringen</trd>,
      <trd>wichsen<klr> (Stiefel)</klr></trd>
    </trdgrp>
    <trd lng="fr">polir</trd>
    <trdgrp lng="it">
      <trd>lisciare</trd>,
      <trd>lucidare</trd>,
      <trd>levigare</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>wrijven</trd>,
      <trd>oppoetsen</trd>
    </trdgrp>
  </snc>
  <snc mrk="polur.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Perfekte <ref cel="labor.el0i">ellabori</ref>, pli delikatigi,
      pli <ref cel="rafin.0i.FIG">rafini</ref>;
      <ref cel="fajl.0i.FIG">fajli</ref>:
      <ekz>
        mi preferas <klr>[&#x2026;]</klr> <tld/>i tiun malbenitan verson de la tria strofo
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>as sian stilon
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        nun li fin<tld/>as sian tradukon de 1001 noktoj
        <fnt>
          <bib>Monato</bib>
          <aut>Nina Kor&jcirc;enevskaja</aut>
          <vrk><url ref="&Monato;/2010/010063.php">Intervjue pri la grafo de Monte-Kristo</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        lasu lin en mian domon, mi lin <tld/>os
        <klr>(severege edukos)</klr>
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fabelo pri iu, kiu migris por ekkoni timon</lok>
        </fnt>;
      </ekz>
      <ekz>
        re<tld/>i siajn batallertojn
        <fnt>
          <bib>StU</bib>
          <lok>Britujo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>polieren</trd>,
      <trd>verfeinern</trd>,
      <trd>ausfeilen</trd>,
      <trd>den letzten <ind>Schliff</ind> geben</trd>
    </trdgrp>
    <trd lng="fr">affiner</trd>
    <trdgrp lng="it">
      <trd>affinare</trd>,
      <trd>raffinare</trd>,
      <trd>limare</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_l;&c_ib;&c_f;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">polir</trd>
  <trd lng="cs">le&scaron;tit</trd>
  <trd lng="en">polish</trd>
  <trdgrp lng="es">
    <trd>pulimentar</trd>,
    <trd>pulir</trd>,
    <trd>bru&ntilde;ir</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x78E8;&#x304F;  <pr>&#x307F;&#x304C;&#x304F;</pr></trd>,
    <trd>&#x7814;&#x78E8;&#x3059;&#x308B;  <pr>&#x3051;&#x3093;&#x307E;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x3064;&#x3084;&#x3092;&#x51FA;&#x3059;  <pr>&#x3064;&#x3084;&#x3092;&#x3060;&#x3059;</pr></trd>,
    <trd>&#x6D17;&#x7DF4;&#x3059;&#x308B;  <pr>&#x305B;&#x3093;&#x308C;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x63A8;&#x6572;&#x3059;&#x308B;  <pr>&#x3059;&#x3044;&#x3053;&#x3046;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">oppoetsen</trd>
  <trdgrp lng="no">
    <trd>polere</trd>,
    <trd>finpusse</trd>
  </trdgrp>
  <trd lng="pt">polir</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_sh;&c_l;&c_i;&c_f;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>cibri&tcaron;</trd>,
    <trd>le&scaron;ti&tcaron;</trd>,
    <trd>vybr&uacute;si&tcaron;</trd>,
    <trd>vyle&scaron;ti&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>polera</trd>,
    <trd>putsa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_l;&c_ib;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_sh;&c_l;&c_ib;&c_f;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_l;&c_o;&c_shch;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_g;&c_l;&c_ja;&c_n;&c_c;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_o;&c_d;&c_i;&c_t;&c_i; &c_g;&c_l;&c_ja;&c_n;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x92E5; <pr>z&#xE8;ng</pr></trd>,
    <trd>&#x9503; <pr>z&#xE8;ng</pr></trd>,
    <trd>&#x64E6;&#x4EAE; <pr>c&#x101;li&#xE0;ng</pr></trd>,
    <trd>&#x78CB; <pr>cu&#x14D;</pr></trd>,
    <trd>&#x78E8;&#x5149; <pr>m&#xF3;gu&#x101;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="polur.0ita">
  <kap><tld/>ita</kap>
  <snc mrk="polur.0ita.TEK">
    <dif>
      Kiun oni <tld/>is &gcirc;is glateco:
      <ekz>
        la malglataj partoj de la plato tenos inkon, ne la <tld/>itaj
        <fnt>
          <aut>J. De Cooman</aut>,
          <vrk><url ref="http://de-cooman.be/jan/eo/etstechniek.htm">
          Gravurteknikoj</url></vrk>,
          <lok>1999-12-12</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">poliert</trd>
    <trd lng="fr">poli<klr> (physiquement)</klr></trd>
    <trdgrp lng="zh">
    <trd>&#x64E6;&#x4EAE; <pr>c&#x101;li&#xE0;ng</pr></trd>,
    <trd>&#x92E5; <pr>z&#xE8;ng</pr></trd>,
    <trd>&#x9503; <pr>z&#xE8;ng</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="polur.0ita.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Prilaborita &gcirc;is kontentiga aspekto, rafinita,
      delikata:
      <ekz>
        li estas frotita kaj <tld/>ita
        <klr>(multe spertinta)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        oni ne disvastigu tekstojn kolbasigitajn en la gugla
        tradukma&scirc;ino sen esti <tld/>itaj
        <fnt>
          <aut>[A. K&uuml;nzli]</aut>,
          <vrk>en: <url ref="http://www.liberafolio.org/2012/forpasis-josi-semer">
          Forpasis Josi &Scirc;emer</url></vrk>,
          <lok>libera folio, 2012-04-05</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>ausgefeilt</trd>,
      <trd>gegl&auml;ttet</trd>,
      <trd>aalglatt</trd>,
      <trd>mit allen <ind>Wasser</ind>n gewaschen</trd>
    </trdgrp>
    <trd lng="fr">raffin&eacute;</trd>
    <trdgrp lng="zh">
    <trd>&#x7CBE;&#x5236; <pr>j&#x12B;ngzh&#xEC;</pr></trd>,
    <trd>&#x7CBE;&#x88FD; <pr>j&#x12B;ngzh&#xEC;</pr></trd>,
    <trd>&#x7CBE;&#x70BC; <pr>j&#x12B;ngli&#xE0;n</pr></trd>,
    <trd>&#x7CBE;&#x7149; <pr>j&#x12B;ngli&#xE0;n</pr></trd>,
    <trd>&#x505B; <pr>ch&#x101;</pr></trd>,
    <trd>&#x516B;&#x9762;&#x5149; <pr>b&#x101;mi&#xE0;ngu&#x101;ng</pr></trd>,
    <trd>&#x5178;&#x96C5; <pr>di&#x1CE;ny&#x1CE;</pr></trd>,
    <trd>&#x5706;&#x6ED1; <pr>yu&#xE1;nhu&#xE1;</pr></trd>,
    <trd>&#x5713;&#x6ED1; <pr>yu&#xE1;nhu&#xE1;</pr></trd>,
    <trd>&#x5F6C;&#x5F6C; <pr>b&#x12B;nb&#x12B;n</pr></trd>,
    <trd>&#x72E1;&#x733E;&#x5730; <pr>ji&#x1CE;ohu&#xE1;de</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_p;&c_a;&c_l;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>,
