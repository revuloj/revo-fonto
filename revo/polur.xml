<drv mrk="polur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="polur.0o.KOMUNE">
    <dif>
      Surfaca <ref tip="super" cel="bril.0o">brilo</ref>, ricevita per frotado:
      <ekz>
        ni nur a&ccirc;etos diamantojn kiuj ricevis sian <tld/>on
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 7</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[li]</klr> kritikis, postulante korektojn a&ubreve; <tld/>on de kelkaj versoj
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LI</lok>
        </fnt>;
      </ekz>
      <ekz>
        kia plebo abomena, sen gusto kaj <tld/>o
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LI</lok>
        </fnt>!
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kion signifas tuta la &gcirc;entileco, socia <tld/>o de monduloj
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>?
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="apretu.0o">apreturo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_l;&c_ja;&c_n;&c_je;&c_c;</trd>,
    <trd>&c_b;&c_l;&c_ja;&c_s;&c_k;</trd>
  </trdgrp>
  <trd lng="cs">le&scaron;t&ecaron;n&iacute;</trd>
  <trdgrp lng="de">
    <trd>Glanz</trd>,
    <trd>Politur <klr>(Glanz)</klr></trd>,
    <trd>Gl&auml;tte</trd>,
    <trd>Schliff<klr> (metaf)</klr></trd>
  </trdgrp>
  <trd lng="es">pulido</trd>
  <trd lng="fr">poli<klr> (subst.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x5149;&#x6CA2;  <pr>&#x3053;&#x3046;&#x305F;&#x304F;</pr></trd>,
    <trd>&#x3064;&#x3084;</trd>
  </trdgrp>
  <trd lng="sk">le&scaron;tenie</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_l;&c_ib;&c_r;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_l;&c_o;&c_shch;&c_ib;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_je;&c_d;&c_je;&c_n;&c_n;&c_ja; &c_g;&c_l;&c_ja;&c_n;&c_c;&c_ju;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5149;&#x6ED1; <pr>gu&#x101;nghu&#xE1;</pr></trd>,
    <trd>&#x4E00;&#x77A5; <pr>y&#x12B;pi&#x113;</pr></trd>,
    <trd>&#x5149;&#x534E; <pr>gu&#x101;nghu&#xE1;</pr></trd>,
    <trd>&#x5149;&#x83EF; <pr>gu&#x101;nghu&#xE1;</pr></trd>,
    <trd>&#x6804; <pr>r&#xF3;ng</pr></trd>,
    <trd>&#x69AE;&#x5149; <pr>r&#xF3;nggu&#x101;ng</pr></trd>,
    <trd>&#x8363;&#x5149; <pr>r&#xF3;nggu&#x101;ng</pr></trd>,
    <trd>&#x77A5; <pr>pi&#x113;</pr></trd>,
    <trd>&#x78E8;&#x5C51; <pr>m&#xF3;xi&#xE8;</pr></trd>
