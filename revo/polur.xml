<drv mrk="polur.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="polur.0ado.KOMUNE">
    <dif>
      Ago <ref tip="dif" cel="polur.0i">poluri</ref>:
      <ekz>
        ili estis <klr>[...]</klr> interesitaj pri la <tld/>ado de la gemoj
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 7</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ke &leftquot;genio estas longa pacienco&rightquot; <klr>[&#x2026;]</klr> tion montris la ses jaroj de
        sen&ccirc;esa <tld/>ado per provado
        <fnt>
          <bib>VivZam</bib>
          <lok>Vivo de Zamenhof</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        pacienca laboro de fajlado kaj da&ubreve;ra verso<tld/>ado
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_g;&c_l;&c_ja;&c_n;&c_c;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_v;&c_y;&c_g;&c_l;&c_ja;&c_n;&c_c;&c_o;&c_w;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_l;&c_a;&c_s;&c_k;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="cs">le&scaron;t&ecaron;n&iacute;</trd>
  <trd lng="de">Polieren</trd>
  <trd lng="es">pulido</trd>
  <trd lng="fr">polissage</trd>
  <trdgrp lng="ja">
    <trd>&#x3064;&#x3084;&#x51FA;&#x3057;  <pr>&#x3064;&#x3084;&#x3060;&#x3057;</pr></trd>,
    <trd>&#x78E8;&#x304D;&#x4E0A;&#x3052;  <pr>&#x307F;&#x304C;&#x304D;&#x3042;&#x3052;</pr></trd>,
