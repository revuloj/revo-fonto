<art mrk="$Id: polur.xml,v 1.27 2024/01/21 12:37:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>polur</rad>/o <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="polur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="polur.0o.KOMUNE">
    <dif>
      Surfaca <ref tip="super" cel="bril.0o">brilo</ref>, ricevita per frotado:
      <ekz>
        ni nur a&ccirc;etos diamantojn kiuj ricevis sian <tld/>on
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 7</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[li]</klr> kritikis, postulante korektojn a&ubreve; <tld/>on de kelkaj versoj
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LI</lok>
        </fnt>;
      </ekz>
      <ekz>
        kia plebo abomena, sen gusto kaj <tld/>o
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LI</lok>
        </fnt>!
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kion signifas tuta la &gcirc;entileco, socia <tld/>o de monduloj
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>?
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="apretu.0o">apreturo</ref>
    </refgrp>
  </snc>
  <trd lng="cs">le&scaron;t&ecaron;n&iacute;</trd>
  <trdgrp lng="de">
    <trd>Glanz</trd>,
    <trd>Politur <klr>(Glanz)</klr></trd>,
    <trd>Gl&auml;tte</trd>,
    <trd>Schliff<klr> (metaf)</klr></trd>
  </trdgrp>
  <trd lng="es">pulido</trd>
  <trd lng="fr">poli<klr> (subst.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#20809;&#27810;  <pr>&#12371;&#12358;&#12383;&#12367;</pr></trd>,
    <trd>&#12388;&#12420;</trd>
  </trdgrp>
  <trd lng="sk">le&scaron;tenie</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_l;&c_ib;&c_r;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_l;&c_o;&c_shch;&c_ib;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_je;&c_d;&c_je;&c_n;&c_n;&c_ja; &c_g;&c_l;&c_ja;&c_n;&c_c;&c_ju;</trd>
  </trdgrp>
</drv>

<drv mrk="polur.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="polur.0ado.KOMUNE">
    <dif>
      Ago <ref tip="dif" cel="polur.0i">poluri</ref>:
      <ekz>
        ili estis <klr>[...]</klr> interesitaj pri la <tld/>ado de la gemoj
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 7</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ke &leftquot;genio estas longa pacienco&rightquot; <klr>[&#8230;]</klr> tion montris la ses jaroj de
        sen&ccirc;esa <tld/>ado per provado
        <fnt>
          <bib>VivZam</bib>
          <lok>Vivo de Zamenhof</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        pacienca laboro de fajlado kaj da&ubreve;ra verso<tld/>ado
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">le&scaron;t&ecaron;n&iacute;</trd>
  <trd lng="de">Polieren</trd>
  <trd lng="es">pulido</trd>
  <trd lng="fr">polissage</trd>
  <trdgrp lng="ja">
    <trd>&#12388;&#12420;&#20986;&#12375;  <pr>&#12388;&#12420;&#12384;&#12375;</pr></trd>,
    <trd>&#30952;&#12365;&#19978;&#12370;  <pr>&#12415;&#12364;&#12365;&#12354;&#12370;</pr></trd>,
    <trd>&#30740;&#30952;  <pr>&#12369;&#12435;&#12414;</pr></trd>,
    <trd>&#27927;&#32244;  <pr>&#12379;&#12435;&#12428;&#12435;</pr></trd>,
    <trd>&#25512;&#25970;  <pr>&#12377;&#12356;&#12371;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="sk">le&scaron;tenie</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_l;&c_ib;&c_r;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_l;&c_o;&c_shch;&c_ib;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_je;&c_d;&c_je;&c_n;&c_n;&c_ja; &c_g;&c_l;&c_ja;&c_n;&c_c;&c_ju;</trd>
  </trdgrp>
</drv>

<drv mrk="polur.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="polur.0i.KOMUNE">
    <dif>
      <ref cel="glat.0igi">Glatigi</ref> kaj <ref tip="super" cel="bril.0igi">briligi</ref>
      ion per frotado:
      <ekz>
        <tld/>i lignon, vitron, marmoron;
      </ekz>
      <ekz>
        ili facetis kaj <tld/>is multekostajn &scirc;tonojn
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili drinkas e&ccirc; <klr>[&#8230;]</klr> e&ccirc; <ind><tld/>kremo</ind><klr>[n]</klr>, enhavanta alkoholon
        kaj uzata por <tld/>i latunon ktp
        <fnt>
          <bib>KrM</bib>
          <lok>31. Ricevis Bandito La&ubreve; Sia Merito.</lok>
        </fnt>;
        <trd lng="de">Politur</trd>
      </ekz>
      <ekz>
        la maljuna re&gcirc;o lasis <tld/>i sian oran kronon
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        granitaj platrokoj, kiujn <klr>[&#8230;]</klr> rabotis kaj <tld/>is la giganta glacio
        <fnt>
          <bib>ViV</bib>
          <lok>IV.</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="facet.0i">faceti</ref>,
      <ref cel="glace.0igi">glaceigi</ref>,
      <ref cel="sxlif.0i">&scirc;lifi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>polieren</trd>,
      <trd>glatt und <ind>gl&auml;nzend</ind> machen</trd>,
      <trd>zum <ind>Gl&auml;nzen</ind> bringen</trd>,
      <trd>wichsen<klr> (Stiefel)</klr></trd>
    </trdgrp>
    <trd lng="fr">polir</trd>
    <trdgrp lng="it">
      <trd>lisciare</trd>,
      <trd>lucidare</trd>,
      <trd>levigare</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>wrijven</trd>,
      <trd>oppoetsen</trd>
    </trdgrp>
  </snc>
  <snc mrk="polur.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Perfekte <ref cel="labor.el0i">ellabori</ref>, pli delikatigi,
      pli <ref cel="rafin.0i.FIG">rafini</ref>;
      <ref cel="fajl.0i.FIG">fajli</ref>:
      <ekz>
        mi preferas <klr>[&#8230;]</klr> <tld/>i tiun malbenitan verson de la tria strofo
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>as sian stilon
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        nun li fin<tld/>as sian tradukon de 1001 noktoj
        <fnt>
          <bib>Monato</bib>
          <aut>Nina Kor&jcirc;enevskaja</aut>
          <vrk><url ref="&Monato;/2010/010063.php"
              >Intervjue pri la grafo de Monte-Kristo</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        lasu lin en mian domon, mi lin <tld/>os
        <klr>(severege edukos)</klr>
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fabelo pri iu, kiu migris por ekkoni timon</lok>
        </fnt>;
      </ekz>
      <ekz>
        re<tld/>i siajn batallertojn
        <fnt>
          <bib>StU</bib>
          <lok>Britujo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>polieren</trd>,
      <trd>verfeinern</trd>,
      <trd>ausfeilen</trd>,
      <trd>den letzten <ind>Schliff</ind> geben</trd>
    </trdgrp>
    <trd lng="fr">affiner</trd>
    <trdgrp lng="it">
      <trd>affinare</trd>,
      <trd>raffinare</trd>,
      <trd>limare</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_l;&c_ib;&c_f;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">polir</trd>
  <trd lng="cs">le&scaron;tit</trd>
  <trd lng="en">polish</trd>
  <trdgrp lng="es">
    <trd>pulimentar</trd>,
    <trd>pulir</trd>,
    <trd>bru&ntilde;ir</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#30952;&#12367;  <pr>&#12415;&#12364;&#12367;</pr></trd>,
    <trd>&#30740;&#30952;&#12377;&#12427;  <pr>&#12369;&#12435;&#12414;&#12377;&#12427;</pr></trd>,
    <trd>&#12388;&#12420;&#12434;&#20986;&#12377;  <pr>&#12388;&#12420;&#12434;&#12384;&#12377;</pr></trd>,
    <trd>&#27927;&#32244;&#12377;&#12427;  <pr>&#12379;&#12435;&#12428;&#12435;&#12377;&#12427;</pr></trd>,
    <trd>&#25512;&#25970;&#12377;&#12427;  <pr>&#12377;&#12356;&#12371;&#12358;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">oppoetsen</trd>
  <trdgrp lng="no">
    <trd>polere</trd>,
    <trd>finpusse</trd>
  </trdgrp>
  <trd lng="pt">polir</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_sh;&c_l;&c_i;&c_f;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>cibri&tcaron;</trd>,
    <trd>le&scaron;ti&tcaron;</trd>,
    <trd>vybr&uacute;si&tcaron;</trd>,
    <trd>vyle&scaron;ti&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>polera</trd>,
    <trd>putsa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_l;&c_ib;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_sh;&c_l;&c_ib;&c_f;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_l;&c_o;&c_shch;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_g;&c_l;&c_ja;&c_n;&c_c;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_o;&c_d;&c_i;&c_t;&c_i; &c_g;&c_l;&c_ja;&c_n;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="polur.0ita">
  <kap><tld/>ita</kap>
  <snc mrk="polur.0ita.TEK">
    <dif>
      Kiun oni <tld/>is &gcirc;is glateco:
      <ekz>
        la malglataj partoj de la plato tenos inkon, ne la <tld/>itaj
        <fnt>
          <aut>J. De Cooman</aut>,
          <vrk><url ref="http://de-cooman.be/jan/eo/etstechniek.htm">
          Gravurteknikoj</url></vrk>,
          <lok>1999-12-12</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">poliert</trd>
    <trd lng="fr">poli<klr> (physiquement)</klr></trd>
  </snc>
  <snc mrk="polur.0ita.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Prilaborita &gcirc;is kontentiga aspekto, rafinita,
      delikata:
      <ekz>
        li estas frotita kaj <tld/>ita
        <klr>(multe spertinta)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        oni ne disvastigu tekstojn kolbasigitajn en la gugla
        tradukma&scirc;ino sen esti <tld/>itaj
        <fnt>
          <aut>[A. K&uuml;nzli]</aut>,
          <vrk>en: <url ref="http://www.liberafolio.org/2012/forpasis-josi-semer">
          Forpasis Josi &Scirc;emer</url></vrk>,
          <lok>libera folio, 2012-04-05</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>ausgefeilt</trd>,
      <trd>gegl&auml;ttet</trd>,
      <trd>aalglatt</trd>,
      <trd>mit allen <ind>Wasser</ind>n gewaschen</trd>
    </trdgrp>
    <trd lng="fr">raffin&eacute;</trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#30952;&#12365;&#12398;&#12363;&#12363;&#12387;&#12383;  <pr>&#12415;&#12364;&#12365;&#12398;&#12363;&#12363;&#12387;&#12383;</pr></trd>,
    <trd>&#12388;&#12420;&#12398;&#12354;&#12427;</trd>,
    <trd>&#27927;&#32244;&#12373;&#12428;&#12383;  <pr>&#12379;&#12435;&#12428;&#12435;&#12373;&#12428;&#12383;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="polur.ne0ita">
  <kap>ne<tld/>ita</kap>
  <snc mrk="polur.ne0ita.TEK">
    <dif>
      Kiun oni ne <tld/>is, kiu restas kruda,
      raspa, malglata:
      <ekz>
        ofte li estis portanta <klr>[&#8230;]</klr> ne<tld/>itajn botojn kaj falantan
        &ccirc;apon
        <fnt>
          <aut>A. Protti de Andrade, M. Costa Nocera</aut>,
          <vrk><url ref="http://lamasson.com.br/biblioteca/biblioteca/pesquisahomeopatica/artikolo00n2nilocairo.htm">
          D-ro Nilo Cairo: vivo kaj verko de de iu homeopato kaj
          edukisto</url></vrk>,
          <lok>[2000]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>unpoliert</trd>,
      <trd>rau</trd>,
      <trd>matt</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>raboteux</trd>,
      <trd>r&ecirc;che</trd>
    </trdgrp>
  </snc>
  <snc mrk="polur.ne0ita.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senkultura, en natura stato, kruda:
      <ekz>
        <ind>ne<tld/>ita homo</ind>.
        <trd lng="fr">ours mal l&ecirc;ch&eacute;</trd>
        <trd lng="nl">ongelikte <ind>beer</ind></trd>
      </ekz>
    </dif>
    <trd lng="de">ungehobelt</trd>
    <trd lng="fr">brut</trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#30952;&#12363;&#12428;&#12390;&#12356;&#12394;&#12356;  <pr>&#12415;&#12364;&#12363;&#12428;&#12390;&#12356;&#12394;&#12356;</pr></trd>,
    <trd>&#27927;&#32244;&#12373;&#12428;&#12390;&#12356;&#12394;&#12356;  <pr>&#12379;&#12435;&#12428;&#12435;&#12373;&#12428;&#12390;&#12356;&#12394;&#12356;</pr></trd>,
    <trd>&#31895;&#37326;&#12394;  <pr>&#12381;&#12420;&#12394;</pr></trd>
  </trdgrp>
</drv>
