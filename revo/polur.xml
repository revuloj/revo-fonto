<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: polur.xml,v 1.25 2023/09/03 12:45:10 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>polur</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="polur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="polur.0o.KOMUNE">
    <dif>
      Surfaca brilo, ricevita per frotado.
    </dif>
  </snc>
  <trd lng="cs">le&scaron;t&ecaron;n&iacute;</trd>
  <trdgrp lng="de">
      <trd>Glanz</trd>,
      <trd>Politur</trd>,
      <trd>Gl&auml;tte</trd>,
      <trd>Schliff<klr> [metaf]</klr></trd>
  </trdgrp>
  <trd lng="es">pulido</trd>
  <trd lng="fr">poli<klr> (subst.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x5149;&#x6CA2;  [<ind>&#x3053;&#x3046;&#x305F;&#x304F;</ind>]</trd>,
    <trd>&#x3064;&#x3084;</trd>
  </trdgrp>
  <trd lng="sk">le&scaron;tenie</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43B;&c_ib;&c_r;&c_u;&c_v;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43B;&#x43E;&c_shch;&c_ib;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43D;&c_a;&c_v;&c_je;&c_d;&c_je;&#x43D;&#x43D;&#x44F; &c_g;&#x43B;&#x44F;&#x43D;&c_c;&#x44E;</trd>
  </trdgrp>
</drv>

<drv mrk="polur.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="polur.0ado.KOMUNE">
    <dif>
      Ago <ref tip="dif" cel="polur.0i">poluri</ref>.
    </dif>
  </snc>
  <trd lng="cs">le&scaron;t&ecaron;n&iacute;</trd>
  <trd lng="es">pulido</trd>
  <trd lng="fr">polissage</trd>
  <trdgrp lng="ja">
    <trd>&#x3064;&#x3084;&#x51FA;&#x3057;  [<ind>&#x3064;&#x3084;&#x3060;&#x3057;</ind>]</trd>,
    <trd>&#x78E8;&#x304D;&#x4E0A;&#x3052;  [<ind>&#x307F;&#x304C;&#x304D;&#x3042;&#x3052;</ind>]</trd>,
    <trd>&#x7814;&#x78E8;  [<ind>&#x3051;&#x3093;&#x307E;</ind>]</trd>,
    <trd>&#x6D17;&#x7DF4;  [<ind>&#x305B;&#x3093;&#x308C;&#x3093;</ind>]</trd>,
    <trd>&#x63A8;&#x6572;  [<ind>&#x3059;&#x3044;&#x3053;&#x3046;</ind>]</trd>
  </trdgrp>
  <trd lng="sk">le&scaron;tenie</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43B;&c_ib;&c_r;&c_u;&c_v;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43B;&#x43E;&c_shch;&c_ib;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43D;&c_a;&c_v;&c_je;&c_d;&c_je;&#x43D;&#x43D;&#x44F; &c_g;&#x43B;&#x44F;&#x43D;&c_c;&#x44E;</trd>
  </trdgrp>
</drv>

<drv mrk="polur.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="polur.0i.KOMUNE">
    <dif>
      Glatigi kaj briligi ion per frotado:
      <ekz>
        <tld/>i lignon, vitron, marmoron.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rabot.0i">raboti</ref>
    </refgrp>
  <trdgrp lng="de">
      <trd>polieren</trd>,
      <trd>glatt und gl&auml;nzend machen</trd>,
      <trd>zum <ind>gl&auml;nzen</ind> bringen</trd>,
      <trd>wichsen<klr> [Stiefel]</klr></trd>
  </trdgrp>
    <trd lng="fr">polir</trd>
    <trdgrp lng="it">
      <trd>lisciare</trd>,
      <trd>lucidare</trd>,
      <trd>levigare</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>wrijven</trd>,
      <trd>oppoetsen</trd>
    </trdgrp>
  </snc>
  <snc mrk="polur.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Perfekte ellabori, plidelikatigi, plirafini;
      <ref tip="vid" cel="fajl.0i">fajli</ref>:
      <ekz>
        <tld/>i sian verkon;
      </ekz>
      <ekz>
        li ne sufi&ccirc;e <tld/>as sian stilon.
      </ekz>
    </dif>
  <trdgrp lng="de">
      <trd>polieren</trd>,
      <trd>verfeinern</trd>,
      <trd>ausfeilen</trd>
  </trdgrp>
    <trd lng="fr">affiner</trd>
    <trdgrp lng="it">
      <trd>affinare</trd>,
      <trd>raffinare</trd>,
      <trd>limare</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_l;&c_ib;&c_f;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">polir</trd>
  <trd lng="cs">le&scaron;tit</trd>
  <trd lng="en">polish</trd>
  <trdgrp lng="es">
    <trd>pulimentar</trd>,
    <trd>pulir</trd>,
    <trd>bru&ntilde;ir</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x78E8;&#x304F;  [<ind>&#x307F;&#x304C;&#x304F;</ind>]</trd>,
    <trd>&#x7814;&#x78E8;&#x3059;&#x308B;  [<ind>&#x3051;&#x3093;&#x307E;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x3064;&#x3084;&#x3092;&#x51FA;&#x3059;  [<ind>&#x3064;&#x3084;&#x3092;&#x3060;&#x3059;</ind>]</trd>,
    <trd>&#x6D17;&#x7DF4;&#x3059;&#x308B;  [<ind>&#x305B;&#x3093;&#x308C;&#x3093;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x63A8;&#x6572;&#x3059;&#x308B;  [<ind>&#x3059;&#x3044;&#x3053;&#x3046;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">oppoetsen</trd>
  <trdgrp lng="no">
    <trd>polere</trd>,
    <trd>finpusse</trd>
  </trdgrp>
  <trd lng="pt">polir</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_sh;&c_l;&c_i;&c_f;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>cibri&tcaron;</trd>,
    <trd>le&scaron;ti&tcaron;</trd>,
    <trd>vybr&uacute;si&tcaron;</trd>,
    <trd>vyle&scaron;ti&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>polera</trd>,
    <trd>putsa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43B;&c_ib;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_sh;&#x43B;&c_ib;&c_f;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43B;&#x43E;&c_shch;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_g;&#x43B;&#x44F;&#x43D;&c_c;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43D;&c_a;&c_v;&#x43E;&c_d;&c_i;&c_t;&c_i; &c_g;&#x43B;&#x44F;&#x43D;&c_je;&c_c;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="polur.0ita">
  <kap><tld/>ita</kap>
  <snc mrk="polur.0ita.TEK">
    <dif>
      Kiun oni <tld/>is &gcirc;is glateco:
      <ekz>
        la malglataj partoj de la plato tenos inkon, ne la <tld/>itaj
        <fnt>
          <aut>J. De Cooman</aut>,
          <vrk><url ref="http://de-cooman.be/jan/eo/etstechniek.htm">
          Gravurteknikoj</url></vrk>,
          <lok>1999-12-12</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">poli<klr> (physiquement)</klr></trd>
  </snc>
  <snc mrk="polur.0ita.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Prilaborita &gcirc;is kontentiga aspekto, rafinita,
      delikata:
      <ekz>
        li estas frotita kaj <tld/>ita
        <klr>(multe spertinta)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        oni ne disvastigu tekstojn kolbasigitajn en la gugla 
        tradukma&scirc;ino sen esti <tld/>itaj
        <fnt>
          <aut>[A. K&uuml;nzli]</aut>,
          <vrk>en: <url ref="http://www.liberafolio.org/2012/forpasis-josi-semer">
          Forpasis Josi &Scirc;emer</url></vrk>,
          <lok>libera folio, 2012-04-05</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">raffin&eacute;</trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x78E8;&#x304D;&#x306E;&#x304B;&#x304B;&#x3063;&#x305F;  [<ind>&#x307F;&#x304C;&#x304D;&#x306E;&#x304B;&#x304B;&#x3063;&#x305F;</ind>]</trd>,
    <trd>&#x3064;&#x3084;&#x306E;&#x3042;&#x308B;</trd>,
    <trd>&#x6D17;&#x7DF4;&#x3055;&#x308C;&#x305F;  [<ind>&#x305B;&#x3093;&#x308C;&#x3093;&#x3055;&#x308C;&#x305F;</ind>]</trd>
  </trdgrp>
</drv>

<drv mrk="polur.ne0ita">
  <kap>ne<tld/>ita</kap>
  <snc mrk="polur.ne0ita.TEK">
    <dif>
      Kiun oni ne <tld/>is, kiu restas kruda,
      raspa, malglata:
      <ekz>
        ofte li estis portanta ... ne<tld/>itajn botojn kaj falantan 
        &ccirc;apon
        <fnt>
          <aut>A. Protti de Andrade, M. Costa Nocera</aut>,
          <vrk><url ref="http://lamasson.com.br/biblioteca/biblioteca/pesquisahomeopatica/artikolo00n2nilocairo.htm">
          D-ro Nilo Cairo: vivo kaj verko de de iu homeopato kaj
          edukisto</url></vrk>,
          <lok>[2000]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>raboteux</trd>,
      <trd>r&ecirc;che</trd>
    </trdgrp>
  </snc>
  <snc mrk="polur.ne0ita.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senkultura, en natura stato, kruda:
      <ekz>
        <ind>ne<tld/>ita homo</ind>.
        <trd lng="fr">ours mal l&ecirc;ch&eacute;</trd>
        <trd lng="nl">ongelikte <ind>beer</ind></trd>
      </ekz>
    </dif>
    <trd lng="fr">brut</trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x78E8;&#x304B;&#x308C;&#x3066;&#x3044;&#x306A;&#x3044;  [<ind>&#x307F;&#x304C;&#x304B;&#x308C;&#x3066;&#x3044;&#x306A;&#x3044;</ind>]</trd>,
    <trd>&#x6D17;&#x7DF4;&#x3055;&#x308C;&#x3066;&#x3044;&#x306A;&#x3044;  [<ind>&#x305B;&#x3093;&#x308C;&#x3093;&#x3055;&#x308C;&#x3066;&#x3044;&#x306A;&#x3044;</ind>]</trd>,
    <trd>&#x7C97;&#x91CE;&#x306A;  [<ind>&#x305D;&#x3084;&#x306A;</ind>]</trd>
  </trdgrp>
</drv>
</art>
