<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>polur</rad>/i
</kap>
<drv mrk="polur.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Glatigi kaj briligi ion per frotado:
      <ekz>
        <tld/>i lignon, vitron, marmoron.
      </ekz>
    </dif>
    <trdgrp lng="nl">
      <trd>wrijven</trd>,
      <trd>oppoetsen</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Perfekte ellabori, plidelikatigi, plirafini:
      <ekz>
        <tld/>i sian verkon;
      </ekz>
      <ekz>
        li ne sufi&ccirc;e <tld/>as sian stilon;
      </ekz>
      <ekz>
        li estas frotita kaj <tld/>ita<fnt>Z</fnt>
        <klr>(multe spertinta)</klr>;
      </ekz>
      <ekz>
        <ind>ne<tld/>ita homo</ind> (senkultura, kruda).
        <trd lng="nl">ongelikte <ind>beer</ind></trd>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fajl.0i">fajli</ref>,
      <ref cel="rabot.0i">raboti</ref>.
    </refgrp>
  </snc>
  <trd lng="de">polieren</trd>
  <trd lng="nl">oppoetsen</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_sh;&c_l;&c_i;&c_f;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
