<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>polur</rad>/o
</kap>

<drv mrk="polur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Surfaca brilo, ricevita per frotado.
    </dif>
  </snc>
  <trd lng="fr">poli<klr> (subst.)</klr></trd>
</drv>

<drv mrk="polur.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Ago <ref tip="dif" cel="polur.0i">poluri</ref>.
    </dif>
  </snc>
  <trd lng="fr">polissage</trd>
</drv>

<drv mrk="polur.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Glatigi kaj briligi ion per frotado:
      <ekz>
        <tld/>i lignon, vitron, marmoron.
      </ekz>
    </dif>
    <trd lng="fr">polir</trd>
    <trdgrp lng="it">
      <trd>lisciare</trd>,
      <trd>lucidare</trd>,
      <trd>levigare</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>wrijven</trd>,
      <trd>oppoetsen</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Perfekte ellabori, plidelikatigi, plirafini:
      <ekz>
        <tld/>i sian verkon;
      </ekz>
      <ekz>
        li ne sufi&ccirc;e <tld/>as sian stilon;
      </ekz>
      <ekz>li estas frotita kaj <tld/>ita
        <klr>(multe spertinta)</klr>
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        <ind>ne<tld/>ita homo</ind> (senkultura, kruda).
        <trd lng="nl">ongelikte <ind>beer</ind></trd>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fajl.0i">fajli</ref>,
      <ref cel="rabot.0i">raboti</ref>.
    </refgrp>
    <trd lng="fr">affiner</trd>
    <trdgrp lng="it">
      <trd>affinare</trd>,
      <trd>raffinare</trd>,
      <trd>limare</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_l;&c_ib;&c_f;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">polir</trd>
  <trd lng="de">polieren</trd>
  <trd lng="en">polish</trd>
  <trd lng="nl">oppoetsen</trd>
  <trd lng="pt">polir</trd>
  <trdgrp lng="no">
    <trd>polere</trd>,
    <trd>finpusse</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_sh;&c_l;&c_i;&c_f;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>polera</trd>,
    <trd>putsa</trd>
  </trdgrp>
</drv>
</art>
