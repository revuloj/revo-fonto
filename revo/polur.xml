<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: polur.xml,v 1.23 2022/03/26 12:56:10 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>polur</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="polur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="polur.0o.KOMUNE">
    <dif>
      Surfaca brilo, ricevita per frotado.
    </dif>
  </snc>
  <trd lng="cs">le&scaron;t&ecaron;n&iacute;</trd>
  <trd lng="es">pulido</trd>
  <trd lng="fr">poli<klr> (subst.)</klr></trd>
  <trd lng="sk">le&scaron;tenie</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43B;&#x456;&#x440;&#x443;&#x432;&#x430;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43B;&#x43E;&#x449;&#x456;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43D;&#x430;&#x432;&#x435;&#x434;&#x435;&#x43D;&#x43D;&#x44F; &#x433;&#x43B;&#x44F;&#x43D;&#x446;&#x44E;</trd>
  </trdgrp>
</drv>

<drv mrk="polur.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="polur.0ado.KOMUNE">
    <dif>
      Ago <ref tip="dif" cel="polur.0i">poluri</ref>.
    </dif>
  </snc>
  <trd lng="cs">le&scaron;t&ecaron;n&iacute;</trd>
  <trd lng="es">pulido</trd>
  <trd lng="fr">polissage</trd>
  <trd lng="sk">le&scaron;tenie</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43B;&#x456;&#x440;&#x443;&#x432;&#x430;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43B;&#x43E;&#x449;&#x456;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43D;&#x430;&#x432;&#x435;&#x434;&#x435;&#x43D;&#x43D;&#x44F; &#x433;&#x43B;&#x44F;&#x43D;&#x446;&#x44E;</trd>
  </trdgrp>
</drv>

<drv mrk="polur.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="polur.0i.KOMUNE">
    <dif>
      Glatigi kaj briligi ion per frotado:
      <ekz>
        <tld/>i lignon, vitron, marmoron.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rabot.0i">raboti</ref>
    </refgrp>
    <trd lng="fr">polir</trd>
    <trdgrp lng="it">
      <trd>lisciare</trd>,
      <trd>lucidare</trd>,
      <trd>levigare</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>wrijven</trd>,
      <trd>oppoetsen</trd>
    </trdgrp>
  </snc>
  <snc mrk="polur.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Perfekte ellabori, plidelikatigi, plirafini;
      <ref tip="vid" cel="fajl.0i">fajli</ref>:
      <ekz>
        <tld/>i sian verkon;
      </ekz>
      <ekz>
        li ne sufi&ccirc;e <tld/>as sian stilon.
      </ekz>
    </dif>
    <trd lng="fr">affiner</trd>
    <trdgrp lng="it">
      <trd>affinare</trd>,
      <trd>raffinare</trd>,
      <trd>limare</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_l;&c_ib;&c_f;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">polir</trd>
  <trd lng="cs">le&scaron;tit</trd>
  <trd lng="de">polieren</trd>
  <trd lng="en">polish</trd>
  <trdgrp lng="es">
    <trd>pulimentar</trd>,
    <trd>pulir</trd>,
    <trd>bru&ntilde;ir</trd>
  </trdgrp>
  <trd lng="nl">oppoetsen</trd>
  <trdgrp lng="no">
    <trd>polere</trd>,
    <trd>finpusse</trd>
  </trdgrp>
  <trd lng="pt">polir</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_sh;&c_l;&c_i;&c_f;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>cibri&tcaron;</trd>,
    <trd>le&scaron;ti&tcaron;</trd>,
    <trd>vybr&uacute;si&tcaron;</trd>,
    <trd>vyle&scaron;ti&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>polera</trd>,
    <trd>putsa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43B;&#x456;&#x440;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x448;&#x43B;&#x456;&#x444;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43B;&#x43E;&#x449;&#x438;&#x442;&#x438;</trd>,
    <trd>&#x433;&#x43B;&#x44F;&#x43D;&#x446;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43D;&#x430;&#x432;&#x43E;&#x434;&#x438;&#x442;&#x438; &#x433;&#x43B;&#x44F;&#x43D;&#x435;&#x446;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="polur.0ita">
  <kap><tld/>ita</kap>
  <snc mrk="polur.0ita.TEK">
    <dif>
      Kiun oni <tld/>is &gcirc;is glateco:
      <ekz>
        la malglataj partoj de la plato tenos inkon, ne la <tld/>itaj
        <fnt>
          <aut>J. De Cooman</aut>,
          <vrk><url ref="http://de-cooman.be/jan/eo/etstechniek.htm">
          Gravurteknikoj</url></vrk>,
          <lok>1999-12-12</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">poli<klr> (physiquement)</klr></trd>
  </snc>
  <snc mrk="polur.0ita.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Prilaborita &gcirc;is kontentiga aspekto, rafinita,
      delikata:
      <ekz>
        li estas frotita kaj <tld/>ita
        <klr>(multe spertinta)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        oni ne disvastigu tekstojn kolbasigitajn en la gugla 
        tradukma&scirc;ino sen esti <tld/>itaj
        <fnt>
          <aut>[A. K&uuml;nzli]</aut>,
          <vrk>en: <url ref="http://www.liberafolio.org/2012/forpasis-josi-semer">
          Forpasis Josi &Scirc;emer</url></vrk>,
          <lok>libera folio, 2012-04-05</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">raffin&eacute;</trd>
  </snc>
</drv>

<drv mrk="polur.ne0ita">
  <kap>ne<tld/>ita</kap>
  <snc mrk="polur.ne0ita.TEK">
    <dif>
      Kiun oni ne <tld/>is, kiu restas kruda,
      raspa, malglata:
      <ekz>
        ofte li estis portanta ... ne<tld/>itajn botojn kaj falantan 
        &ccirc;apon
        <fnt>
          <aut>A. Protti de Andrade, M. Costa Nocera</aut>,
          <vrk><url ref="http://lamasson.com.br/biblioteca/biblioteca/pesquisahomeopatica/artikolo00n2nilocairo.htm">
          D-ro Nilo Cairo: vivo kaj verko de de iu homeopato kaj
          edukisto</url></vrk>,
          <lok>[2000]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>raboteux</trd>,
      <trd>r&ecirc;che</trd>
    </trdgrp>
  </snc>
  <snc mrk="polur.ne0ita.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senkultura, en natura stato, kruda:
      <ekz>
        <ind>ne<tld/>ita homo</ind>.
        <trd lng="fr">ours mal l&ecirc;ch&eacute;</trd>
        <trd lng="nl">ongelikte <ind>beer</ind></trd>
      </ekz>
    </dif>
    <trd lng="fr">brut</trd>
  </snc>
</drv>
</art>
