<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: konfidenci.xml,v 1.7 2019/10/13 12:10:19 revo Exp $">
<kap>
  <rad>konfidenci</rad>/o <fnt><bib>Far2</bib></fnt>
</kap>
<drv mrk="konfidenci.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="konfidenci.0o.konfidenco">
    <dif>
      <ref tip="dif" cel="konfid1.0o">Konfidenco</ref>:
      <ekz>
        mi entute ne havas kutimon fari <tld/>ojn
        <fnt><bib>PatrojFiloj</bib><lok>&ccirc;ap. 17</lok></fnt>;
      </ekz>
      <ekz>
        mi diras tion al via ekscelenco tute <tld/>e, kiel al alta &scirc;tata oficisto
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        li plendis <tld/>e al la edzino
        <fnt><bib>PatrojFiloj</bib><lok>&ccirc;ap. 27, p. 191</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">confidentie</trd>
  <trdgrp lng="pl">
    <trd>konfidencja</trd>,
    <trd>poufne zwierzenie</trd>
  </trdgrp>   
  <trd lng="pt">confid&ecirc;ncia</trd>
  <trdgrp lng="ro">
    <trd>credin&#539;&abreve;</trd>,
    <trd>m&abreve;rturisire  confiden&#539;ial&abreve;</trd>
  </trdgrp>
</drv>
</art>
