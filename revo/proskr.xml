<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: proskr.xml,v 1.15 2022/03/26 15:53:38 revo Exp $">
<kap>
  <rad>proskrib</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="proskr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="proskr.0i.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Senju&gcirc;e <ref tip="super" cel="ekzil.0i">ekzili</ref>, en tempo de
      ekstera a&ubreve; interna milito:
      <ekz>
        &leftquot;Maskerado &ccirc;irka&ubreve; la morto&rightquot; <klr>[&#x2026;]</klr> vive kaj persone bildigas, kiel estis
        esti judo <tld/>ita en naziokupita Hungarlando
        <fnt>
          <bib>Plumamikoj</bib>
          <lok>Prozo nefikcia</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>poslat do vyhnanstv&iacute;</trd>,
    <trd>proskribovat</trd>,
    <trd>vyhostit</trd>,
    <trd>zavrhnout</trd>
  </trdgrp>
  <trd lng="de">&auml;chten</trd>
  <trdgrp lng="fr">
    <trd>proscrire</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&aacute;m&udblac;z <klr tip="ind">(&iacute;t&eacute;let n&eacute;lk&uuml;l)</klr></trd>,
    <trd>t&ouml;rv&eacute;nyen k&iacute;v&uuml;l helyez</trd>
    <trd>proskrib&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">vogelvrij verklaren</trd>
  <trdgrp lng="pt">
    <trd>proscrever</trd>,
    <trd>exilar</trd>,
    <trd>banir</trd>,
    <trd>degredar</trd>,
    <trd>deportar</trd>    
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_g;&c_n;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_s;&c_l;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_malmol;&c_ja;&c_v;&c_i;&c_t;&c_mol; &c_v;&c_n;&c_je; &c_z;&c_a;&c_k;&c_o;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>posla&tcaron; do vyhnanstva</trd>,
    <trd>vyhosti&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x438;&#x433;&#x430;&#x43D;&#x44F;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x438;&#x441;&#x438;&#x43B;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43E;&#x433;&#x43E;&#x43B;&#x43E;&#x448;&#x443;&#x432;&#x430;&#x442;&#x438; &#x43F;&#x43E;&#x437;&#x430; &#x437;&#x430;&#x43A;&#x43E;&#x43D;&#x43E;&#x43C;</trd>
  </trdgrp>
</drv>

<drv mrk="proskr.0o">
  <kap><tld/>o</kap>
  <snc mrk="proskr.0o.JUR">
    <dif>
      Senju&gcirc;a <ref tip="super" cel="kondam.0o">kondamno</ref> 
      al <ref cel="ekzil.0o">ekzilo</ref>:
      <ekz>
        &ccirc;ie oni diris ke <nom>Alan</nom> pafis la kuglon kaj oni jam faris 
        <tld/>on kontra&ubreve; li
        <fnt><aut>R. L. Stevenson, trad. A. Goodheir</aut>, 
        <vrk>Forkaptita</vrk>, <lok>Kardo, 1976</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="proskr1.0o">proskripcio</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Proskription</trd>,
    <trd>&Auml;chtung</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>proscription</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&aacute;m&udblac;zet&eacute;s <klr tip="ind">(&iacute;t&eacute;let n&eacute;lk&uuml;li)</klr></trd>,
    <trd>t&ouml;rv&eacute;nyen k&iacute;v&uuml;l helyez&eacute;s</trd>
    <trd>proskripci&oacute;</trd>
  </trdgrp>  
  <trdgrp lng="nl">
    <trd>proscriptie</trd>,
    <trd>vogelvrijverklaring</trd>
  </trdgrp>
  <trd lng="pt">proscri&ccedil;&atilde;o, banimento, desterro, degredo,
  deporta&ccedil;&atilde;o</trd>
    <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_g;&c_n;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_v;&c_y;&c_s;&c_y;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_k;&c_r;&c_i;&c_p;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>proskripcia</trd>,
    <trd>vypovedanie do vyhnanstva</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>proskripce</trd>,
    <trd>vyhnanstv&iacute;</trd>,
    <trd>vyho&scaron;t&ecaron;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x438;&#x433;&#x43D;&#x430;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x432;&#x438;&#x441;&#x438;&#x43B;&#x43A;&#x430;</trd>,
    <trd>&#x43F;&#x440;&#x43E;&#x441;&#x43A;&#x440;&#x438;&#x43F;&#x446;&#x456;&#x44F;</trd>,
    <trd>&#x43E;&#x433;&#x43E;&#x43B;&#x43E;&#x448;&#x435;&#x43D;&#x43D;&#x44F; &#x43F;&#x43E;&#x437;&#x430; &#x437;&#x430;&#x43A;&#x43E;&#x43D;&#x43E;&#x43C;</trd>
  </trdgrp>
</drv>
</art>
