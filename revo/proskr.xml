<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>proskrib</rad>/i <fnt>Z</fnt></kap>
<drv mrk="proskr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Senju&gcirc;e <ref tip="super" cel="ekzil.0i">ekzili</ref>, en tempo de ekstera
      a&ubreve; interna milito.
    </dif>
  </snc>
  <trd lng="de">&auml;chten</trd>
  <trdgrp lng="fr">
    <trd>proscrire</trd>
  </trdgrp>
  <trd lng="nl">vogelvrij verklaren</trd>
</drv>

<drv mrk="proskr.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Senju&gcirc;a kondamno al ekzilo.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="proskr1.0o">proskripcio</ref>
  </refgrp>
  <trdgrp lng="de">
    <trd>Proskription</trd>,
    <trd>&Auml;chtung</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>proscription</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>proscriptie</trd>,
    <trd>vogelvrijverklaring</trd>
  </trdgrp>
</drv>
</art>
