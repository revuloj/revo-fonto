<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: magnitud.xml,v 1.12 2024/01/16 09:22:23 revo Exp $">
<kap>
  <rad>magnitud</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="magnitud.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="magnitud.0o.SCI">
    <dif>
      <ref tip="dif" cel="fort.0o.intenseco">Forto</ref>,
      <ref tip="dif" cel="grand.0o.FIZ">grando</ref>
      en kelkaj sciencaj <ref tip="malprt" cel="skal.0o.absoluta">skalo</ref>j
      precipe logaritmaj.
    </dif>
    <subsnc mrk="magnitud.0o.GEOL">
      <uzo tip="fak">GEOL</uzo>
      <dif>
        Energio ellasata de tertremo:
        <ekz>
          estis registritaj neniuj okazoj kun pli granda <tld/>o ol 1
          <fnt>
            <aut>V. Pistoli</aut>,
            <vrk><url ref="&STEB;geologio/sismo-alban/sismo-alban.htm">
            Sismologio kaj kontra&ubreve;-tertrema protekto en
            Albanio</url></vrk>,
            <lok>Scienca Revuo, 1992:2, (161), p. 21a-26a</lok>
          </fnt>;
        </ekz>
        <ekz>
          ene de 30 jaroj okazos sub la metropolo je probableco de 70&#8239;% tertremo de
          separanga <tld/>o
          <fnt>
            <bib>Monato</bib>
            <aut>Isikawa Takasi</aut>
            <vrk><url ref="&Monato;/2014/011255.php"
                >Pikas la ludoj</url></vrk>
            <lok>2014</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="nl">momentmagnitudeschaal</trd>
      <trd lng="no">momentmagnitude</trd>
    </subsnc>
    <subsnc mrk="magnitud.0o.AST">
      <uzo tip="fak">AST</uzo>
      <dif>
        Brileco de astro:
        <ekz>
          <tld/>o reprezentas stelan brilecon, sed ne &gcirc;ian grandecon
          <fnt>
            <aut>AEK</aut>,
            <vrk><url ref="http://esperanto.org/AEK/amator/magnitudo.html">
            Kiel facile mezuri stelan magnitudon?</url></vrk>,
            <lok>[2000]</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="nl">magnitude</trd>
      <trd lng="no">st&oslash;rrelsesklasse</trd>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="amplit.0o">amplitudo</ref>,
      <ref cel="grad.0o.SCI">grado<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_g;&c_n;&c_ib;&c_t;&c_u;&c_d;&c_a;</trd>,
    <trd>&c_s;&c_ib;&c_l;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">magnitudo</trd>
  <trdgrp lng="de">
    <trd>Magnitude</trd>,
    <trd>Gr&ouml;&szlig;enordnung</trd>
  </trdgrp>
  <trd lng="fr">magnitude</trd>
  <trd lng="ja">&#12510;&#12464;&#12491;&#12481;&#12517;&#12540;&#12489;  <pr>&#12414;&#12368;&#12395;&#12385;&#12421;&#12540;&#12393;</pr></trd>
  <trd lng="nl">magnitude</trd>
  <trd lng="no">magnitude</trd>
  <trd lng="pl">magnituda</trd>
  <trd lng="ro">magnitudine</trd>
  <trd lng="sk">magnit&uacute;da</trd>
  <trd lng="uk">&c_s;&c_i;&c_l;&c_a; &c_s;&c_v;&c_ib;&c_t;&c_ib;&c_n;&c_n;&c_ja; &c_n;&c_je;&c_b;&c_je;&c_s;&c_n;&c_i;&c_h; &c_t;&c_ib;&c_l;</trd>
</drv>
</art>

