<art mrk="$Id: menhir.xml,v 1.22 2023/11/21 21:23:27 revo Exp $">
<kap>
  <rad>menhir</rad>/o
  <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="menhir.0o">
  <kap><tld/>o</kap>
  <snc mrk="menhir.0o.ARKE">
    <uzo tip="fak">ARKE</uzo>
    <dif>
      <ref cel="megali.0o">Megalit</ref>a monumento starigita vertikale:
      <ekz>
        la <tld/>oj, de prajarcentoj starantaj garde sur la bordoj, estis nur fantomoj
        <fnt>
          <aut>W&lstroke;adys&lstroke;aw Reymont, trad. Kabe</aut>
          <vrk><url ref="&Steloj;lasta/index.html">La Lasta</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <ctl>Merlo sur <tld/>o</ctl>
        <klr>(poemaro de Alberto <nom>Goodheir</nom>)</klr>
        <fnt><aut>Goodheir Albert</aut>, <vrk>Merlo sur menhiro.
        Glasgovo: Glasgova Esperanto-Societo, 1974</vrk></fnt>.
      </ekz>
    </dif>
    <refgrp tip="super">
      <ref cel="megali.0o">megalito</ref>
    </refgrp>
    <bld
      lok="&WCU;/thumb/a/a8/Nages_menhir_du_Tribi.JPG/640px-Nages_menhir_du_Tribi.JPG"
      prm="CC BY-SA 4.0"
      ><tld/>o de Tribi en Nages, Francujo
        <fnt>
          <aut>Fagairolles 34, propra verko</aut>
          <lok><url ref="&WCW;/File:Nages_menhir_du_Tribi.JPG"
              >https://commons.wikimedia.org/wiki/File:Nages_menhir_du_Tribi.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_n;&c_g;&c_ib;&c_r;</trd>
  <trd lng="bg">&c_m;&c_je;&c_n;&c_h;&c_i;&c_r; </trd>
  <trdgrp lng="br">
    <trd>maen-hir</trd>,
    <trd>peulvan</trd>
  </trdgrp>
  <trd lng="ca">menhir</trd>
  <trd lng="cs">menhir</trd>
  <trdgrp lng="de">
    <trd>H&uuml;nenstein</trd>,
    <trd>Menhir</trd>
  </trdgrp>
  <trd lng="en">menhir</trd>
  <trd lng="es">menhir</trd>
  <trdgrp lng="fr">
    <trd>menhir</trd>,
    <trd>pierre lev&eacute;e</trd>
  </trdgrp>
  <trd lng="hu">menhir</trd>
  <trdgrp lng="ja">
    <trd>&#12513;&#12531;&#12498;&#12523;</trd>,
    <trd>&#31435;&#30707;  <pr>&#12383;&#12390;&#12356;&#12375;</pr></trd>
  </trdgrp>
  <trd lng="nl">menhir</trd>
  <trd lng="pt">menir</trd>
  <trd lng="ru">&c_m;&c_je;&c_n;&c_g;&c_i;&c_r;</trd>
  <trd lng="sk">menhir</trd>
  <trd lng="uk">&c_m;&c_je;&c_n;&c_g;&c_ib;&c_r; (&c_p;&c_a;&c_m;&#8217;&c_ja;&c_t;&c_n;&c_i;&c_k;)</trd>
</drv>
</art>
