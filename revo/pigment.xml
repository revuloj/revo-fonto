<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pigment.xml,v 1.8 2019/06/30 07:43:29 revo Exp $">
<kap>
  <rad>pigment</rad>/o <fnt>Z</fnt> <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="pigment.0o">
  <kap><tld/>o</kap>
  <snc mrk="pigment.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      Natura, kolora substanco de vivuloj:
      <ekz>
        klorofilo estas la &ccirc;efa <tld/>o de la fotosintezantaj plantoj
        <fnt>
          <aut>Vikipedio</aut>
          <vrk><url
          ref="http://eo.wikipedia.org/wiki/Klorofilo">Klorofilo</url></vrk>,
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="pigment.0o.KEM">
  <uzo tip="fak">KEM</uzo>
    <dif>
      Kolora substanco de farbo, ordinare konsistanta el
      subtilaj eroj, kiuj ne solvi&gcirc;as sed emulsii&gcirc;as
      en la ligento:
      <ekz>
        farboj konsistas &ccirc;efe el oleo la&ubreve;eble rapide seki&gcirc;anta kaj el
        koloraj <tld/>oj
        <fnt>
          <aut>Vikipedio</aut>
          <vrk><url
          ref="http://eo.wikipedia.org/wiki/Farbo">Farbo</url></vrk>,
          <lok>2013-03-08</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>barvivo</trd>,
    <trd>pigment</trd>
  </trdgrp>
  <trd lng="fr">pigment</trd>
  <trdgrp lng="sk">
    <trd>farbivo</trd>,
    <trd>pigment</trd>
  </trdgrp>
</drv>

<drv mrk="pigment.0a">
  <kap><tld/>a</kap>
  <snc mrk="pigment.0a.BIO">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        por produkti iometon da diversaj el&ccirc;erpitaj farboj mi bezonas
        ser&ccirc;i likvajn kaj <tld/>ajn ingredien<klr>c</klr>ojn
        <fnt>
          <aut>J. Camacho</aut>
          <vrk><url
          ref="http://ebooks.translaten.com/eo-ca/camacho/la-majstro-kaj-martinelli.html">La
          Majstro kaj Martinelli</url></vrk>
          <lok>1993</lok>
        </fnt>;
      </ekz>
      <ekz>
        denaska <tld/>a nevuso
        <fnt>
          <aut>Mu Binghua</aut>
          <vrk><url
          ref="https://www.monato.be/2002/007897.html">Operacio
          sen precedenco</url></vrk>
          <bib>Monato</bib>
          <lok>2002</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>  
  <trd lng="fr">pigmentaire</trd>
</drv>

<drv mrk="pigment.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pigment.0i.BIO">
    <dif>
      Kolorigi per <tld/>o.
    </dif>
  </snc>
  <trd lng="cs">pigmentovat</trd>
  <trd lng="fr">pigmenter</trd>
  <trd lng="sk">pigmentova&tcaron;</trd>
</drv>

<drv mrk="pigment.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc mrk="pigment.0igxo.BIO">
    <dif>
      Amasi&gcirc;o da <tld/>oj en la organaj histoj:
      <ekz>
        tiu <tld/>i&gcirc;o plii&gcirc;as per la sunlumo
        <fnt>
          <aut>Vikipedio</aut>,
          <vrk><url
          ref="http://eo.wikipedia.org/wiki/Efelido">Efelido</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">pigmentation</trd>
</drv>

<drv mrk="pigment.hiper0igxo">
  <kap>hiper<tld/>i&gcirc;o</kap>
  <snc mrk="pigment.hiper0igxo.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <tld lit="P"/>i&gcirc;o super la normala kvanto:
      <ekz>
        lentugo estas ekzemplo de loka hiper<tld/>i&gcirc;o
        <fnt>
          <aut>Vikipedio</aut>,
          <vrk><url
          ref="http://eo.wikipedia.org/wiki/Ha&ubreve;to">Ha&ubreve;to</url></vrk>
        </fnt>
      </ekz>.
    </dif>
  </snc>
  <trd lng="fr">hyperpigmentation</trd>
</drv>
 </art>

