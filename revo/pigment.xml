<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pigment.xml,v 1.15 2024/02/17 06:43:07 revo Exp $">
<kap>
  <rad>pigment</rad>/o <fnt>Z</fnt> <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="pigment.0o">
  <kap><tld/>o</kap>
  <snc mrk="pigment.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      Natura, kolora substanco de vivuloj:
      <ekz>
        klorofilo estas la &ccirc;efa <tld/>o de la fotosintezantaj plantoj
        <fnt>
          <aut>Vikipedio</aut>
          <vrk><url ref="&Viki;/Klorofilo">Klorofilo</url></vrk>,
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="pigment.0o.KEM">
  <uzo tip="fak">KEM</uzo>
    <dif>
      Kolora substanco de farbo, ordinare konsistanta el
      subtilaj eroj, kiuj ne solvi&gcirc;as sed emulsii&gcirc;as
      en la ligento:
      <ekz>
        farboj konsistas &ccirc;efe el oleo la&ubreve;eble rapide seki&gcirc;anta kaj el
        koloraj <tld/>oj
        <fnt>
          <aut>Vikipedio</aut>
          <vrk><url ref="&Viki;/Farbo">Farbo</url></vrk>,
          <lok>2013-03-08</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ib;&c_g;&c_m;&c_e;&c_n;&c_t;</trd>,
    <trd>&c_f;&c_a;&c_r;&c_b;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>barvivo</trd>,
    <trd>pigment</trd>
  </trdgrp>
  <trd lng="fr">pigment</trd>
  <trdgrp lng="ja">
    <trd>&#x8272;&#x7D20;  <pr>&#x3057;&#x304D;&#x305D;</pr></trd>,
    <trd>&#x9854;&#x6599;  <pr>&#x304C;&#x3093;&#x308A;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>barwnik</trd>,
    <trd>pigment</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>colorant</trd>,
    <trd>pigment</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>farbivo</trd>,
    <trd>pigment</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&c_ib;&c_gu;&#x43C;&c_je;&#x43D;&c_t;</trd>
  <trd lng="zh">&#x8272;&#x7D20; <pr>s&#xE8;s&#xF9;</pr></trd>
</drv>

<drv mrk="pigment.0a">
  <kap><tld/>a</kap>
  <snc mrk="pigment.0a.BIO">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        por produkti iometon da diversaj el&ccirc;erpitaj farboj mi bezonas
        ser&ccirc;i likvajn kaj <tld/>ajn ingredien<klr>c</klr>ojn
        <fnt>
          <aut>J. Camacho</aut>
          <vrk><url ref="http://ebooks.translaten.com/eo-ca/camacho/la-majstro-kaj-martinelli.html">La
          Majstro kaj Martinelli</url></vrk>
          <lok>1993</lok>
        </fnt>;
      </ekz>
      <ekz>
        denaska <tld/>a nevuso
        <fnt>
          <aut>Mu Binghua</aut>
          <vrk><url ref="&Monato;/2002/007897.html">Operacio
          sen precedenco</url></vrk>
          <bib>Monato</bib>
          <lok>2002</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>  
  <trd lng="be">&c_p;&c_ib;&c_g;&c_m;&c_e;&c_n;&c_t;&c_n;&c_y;</trd>
  <trd lng="fr">pigmentaire</trd>
  <trd lng="pl">pigmentowy</trd>
  <trd lng="ro">pigmentare</trd>
  <trd lng="uk">&#x43F;&c_ib;&c_gu;&#x43C;&c_je;&#x43D;&c_t;&#x43D;&c_i;&c_j;</trd>
</drv>

<drv mrk="pigment.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pigment.0i.BIO">
    <dif>
      Kolorigi per <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_g;&c_m;&c_e;&c_n;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">pigmentovat</trd>
  <trd lng="fr">pigmenter</trd>
  <trd lng="ja">&#x9854;&#x6599;&#x3067;&#x7740;&#x8272;&#x3059;&#x308B;  <pr>&#x304C;&#x3093;&#x308A;&#x3087;&#x3046;&#x3067;&#x3061;&#x3083;&#x304F;&#x3057;&#x3087;&#x304F;&#x3059;&#x308B;</pr></trd>
  <trd lng="pl">pigmentowa&cacute;</trd>
  <trd lng="ro">pigmenta</trd>
  <trd lng="sk">pigmentova&tcaron;</trd>
  <trd lng="uk">&#x43F;&c_ib;&c_gu;&#x43C;&c_je;&#x43D;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="pigment.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc mrk="pigment.0igxo.BIO">
    <dif>
      Amasi&gcirc;o da <tld/>oj en la organaj histoj:
      <ekz>
        tiu <tld/>i&gcirc;o plii&gcirc;as per la sunlumo
        <fnt>
          <aut>Vikipedio</aut>,
          <vrk><url ref="&Viki;/Efelido">Efelido</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_g;&c_m;&c_e;&c_n;&c_t;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trd lng="fr">pigmentation</trd>
  <trdgrp lng="pl">
    <trd>pigmentacja</trd>,
    <trd>zabarwienie</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>pigmentare</trd>,
    <trd>culoare</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&c_ib;&c_gu;&#x43C;&c_je;&#x43D;&c_t;&c_a;&c_c;&c_ib;&#x44F;</trd>
</drv>

<drv mrk="pigment.hiper0igxo">
  <kap>hiper<tld/>i&gcirc;o</kap>
  <snc mrk="pigment.hiper0igxo.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <tld lit="P"/>i&gcirc;o super la normala kvanto:
      <ekz>
        lentugo estas ekzemplo de loka hiper<tld/>i&gcirc;o
        <fnt>
          <aut>Vikipedio</aut>,
          <vrk><url ref="&Viki;/Ha&ubreve;to">Ha&ubreve;to</url></vrk>
        </fnt>
      </ekz>.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_p;&c_e;&c_r;&c_p;&c_ib;&c_g;&c_m;&c_e;&c_n;&c_t;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trd lng="fr">hyperpigmentation</trd>
  <trd lng="pl">przebarwienie</trd>
  <trd lng="ro">decolorare</trd>
</drv>
 </art>

