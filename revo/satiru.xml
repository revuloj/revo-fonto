<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: satiru.xml,v 1.20 2023/09/03 12:09:44 revo Exp $">
<kap>
  <rad>satirus</rad>/o
  <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="satiru.0o">
  <kap><tld/>o</kap>
  <snc mrk="satiru.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Arbara duondio de la greka mitologio:
      <ekz>
        la verkisto figuris lin kapropieda <tld/>o
        <fnt><bib>SatirRak</bib>, <lok>logos</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="satiru.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Viro voluptema kaj cinika:
      <ekz>
        &scirc;i cedis al lia postulado, scivolante &ccirc;u li tamen estas <tld/>o
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_t;&c_y;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>prostop&aacute;&scaron;n&iacute;k <klr>(p&rcaron;en.)</klr></trd>,
    <trd>satyr</trd>
  </trdgrp>
  <trd lng="de">Satyr</trd>
  <trd lng="en">satyr</trd>
  <trd lng="es">s&aacute;tiro</trd>
  <trd lng="fr">satyre</trd>
  <trd lng="hu">szat&iacute;r</trd>
  <trd lng="it">satiro</trd>
  <trdgrp lng="ja">
    <trd>&#x30B5;&#x30C6;&#x30E5;&#x30ED;&#x30B9;</trd>,
    <trd>&#x597D;&#x8272;&#x5BB6;  [<ind>&#x3053;&#x3046;&#x3057;&#x3087;&#x304F;&#x304B;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">satyr</trd>
  <trd lng="pl">satyr</trd>
  <trd lng="ru">&c_s;&c_a;&c_t;&c_i;&c_r;</trd>
  <trd lng="sk">satyr</trd>
  <trd lng="uk">&#x441;&#x430;&#x442;&#x438;&#x440;</trd>
</drv>
</art>
