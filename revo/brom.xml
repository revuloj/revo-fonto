<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>brom</rad>/o
</kap>
<drv mrk="brom.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <mlg kod="IUPAC">Br</mlg>
  <snc>
  <dif>
    &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
    lst="voko:&hcirc;emiaj_elementoj" val="35">elemento</ref> kun 
    atomnumero 35, atompezo 79,904; 
    <ref tip="super" cel="metalo.0o">metaloido</ref>, kies kompona&jcirc;oj 
    servas en medicino kaj en fotografado. 
  </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_o;&c_m;</trd>
  <trd lng="bg">&c_b;&c_r;&c_o;&c_m;</trd>     
  <trd lng="br">brom</trd>
  <trd lng="ca">brom</trd>
  <trd lng="de">Brom</trd>
  <trd lng="en">bromine</trd>
  <trd lng="es">bromo</trd>
  <trd lng="fa">&a_b;&a_r;&a_m;</trd>
  <trd lng="fr">brome</trd>
  <trd lng="hu">br&oacute;m</trd>
  <trd lng="it">bromo</trd>
  <trd lng="la">Bromum</trd>
  <trd lng="nl">broom</trd>
  <trd lng="pl">brom</trd>
  <trd lng="pt">bromo</trd>
  <trd lng="ru">&c_b;&c_r;&c_o;&c_m;</trd>
  <trd lng="sk">br&oacute;m</trd>
  <trd lng="sv">brom</trd>
</drv>
</art>
