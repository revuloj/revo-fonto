<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>ekzekut</rad>/i
</kap>
<drv mrk="ekzeku.0i">
   <kap><ofc>*</ofc><tld/>i</kap>
   <uzo tip="fak">JUR</uzo>
   <gra><vspec>tr</vspec></gra>
   <snc>
      <dif>
         Le&gcirc;e mortigi krimulon:
         <ekz>
            Longmargareto estis kondukita al la <ind><tld/>ejo</ind>
            <fnt><bib>Fab4</bib>, <lok>Historio el la dunoj</lok></fnt>.
            <trdgrp lng="be">
               <trd>&c_k;&c_a;&c_t;&c_o;&c_w;&c_n;&c_ja;</trd>,
               <trd>&c_z;&c_a;&c_s;&c_mol;&c_c;&c_je;&c_n;&c_a;&c_k;</trd>
            </trdgrp>
         </ekz>
      </dif>
     <trd lng="be">&c_p;&c_a;&c_k;&c_a;&c_r;&c_a;&c_c;&c_mol;
     &c_s;&c_mol;&c_m;&c_je;&c_r;&c_c;&c_ju;</trd>
     <trdgrp lng="ca">
       <trd>executar</trd>,
       <trd>ajusticiar</trd>
     </trdgrp>
     <trdgrp lng="de">
        <trd>hinrichten</trd>,
        <trd>exekutieren</trd>
     </trdgrp>
     <trd lng="en">execute</trd>
     <trd lng="es">ejecutar</trd>
     <trd lng="hu">kiv&eacute;gez</trd>
     <trdgrp lng="pl">
       <trd>wykona&cacute; <ind>egezkucj&eogonek;</ind></trd>,
       <trd>wykona&cacute; <ind>kar&eogonek; &sacute;mierci</ind></trd>,
       <trd>straci&cacute;<klr> (kogo&sacute;)</klr></trd>
     </trdgrp>
     <trd lng="pt">executar</trd>
     <trd lng="ru">&c_k;&c_a;&c_z;&c_n;&c_i;&c_t;&c_mol;</trd>
     <trd lng="fr">ex&eacute;cuter<klr tip="ind"> (un
     condamn&eacute;)</klr></trd>
     <trd lng="sv">avr&auml;tta</trd>
   </snc>
   <rim mrk="ekzeku.0i.RIM_SIM">
     Ne konfuzu kun 
     <ref tip="vid" cel="ekzekuci.0i">ekzekucii</ref>, 
     kiu signifas &leftquot;plenumi decidon&rightquot;.
   </rim>
</drv>

<drv mrk="ekzeku.0o">
   <kap>
     <ofc>*</ofc>
     <tld/>o
   </kap>
   <uzo tip="fak">JUR</uzo>
   <gra><vspec>tr</vspec></gra>
   <snc>
      <dif>
         Le&gcirc;a mortigo de krimulo:
         <ekz>
            prokrasti la <tld/>on;
         </ekz>
      </dif>
      <trd
      lng="be">&c_p;&c_a;&c_k;&c_a;&c_r;&c_a;&c_n;&c_mol;&c_n;&c_je;
      &c_s;&c_mol;&c_m;&c_je;&c_r;&c_c;&c_ju;</trd>
      <trdgrp lng="ca">
        <trd>execuci&oacute;</trd>,
        <trd>ajusticiament</trd>
      </trdgrp>
      <trd lng="en">execution</trd>
      <trd lng="es">ejecuci&oacute;n</trd>
      <trd lng="fr">ex&eacute;cution<klr tip="ind"> (d'un
      condamn&eacute;)</klr></trd>
      <trd lng="hu">kiv&eacute;gz&eacute;s</trd>
      <trdgrp lng="pl">
        <trd>egzekucja</trd>,
        <trd>stracenie</trd>,
        <trd>ka&zacute;&nacute;</trd>
      </trdgrp>
      <trd lng="pt">execu&ccedil;&atilde;o</trd>
   </snc>
</drv>

<drv mrk="ekzeku.0isto">
   <kap><tld/>isto</kap>
   <snc>
      <dif>
         Homo, kies metio estas <tld/>i.
      </dif>
      <trd lng="be">&c_k;&c_a;&c_t;</trd>
      <trd lng="ca">botx&iacute;</trd>
      <trdgrp lng="de">
         <trd>Henker</trd>,
         <trd>Scharfrichter</trd>
      </trdgrp>
      <trdgrp lng="en">
         <trd>executioner</trd>,
         <trd>hangman (p.p. pendumado)</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>bourreau<klr tip="ind"> (ex&eacute;cuteur)</klr></trd>,
        <trd>ex&eacute;cuteur<klr tip="ind">
        (bourreau)</klr></trd></trdgrp>
      <trdgrp lng="hu">
         <trd>h&oacute;h&eacute;r</trd>,
         <trd>bak&oacute;</trd>
      </trdgrp>
      <trd lng="pl">kat</trd>
      <trd lng="ru">&c_p;&c_a;&c_l;&c_a;&c_ch;</trd>
   </snc>
</drv>

<drv mrk="ekzeku.elektro0i">
   <kap>elektro<tld/>i</kap>
   <uzo tip="fak">JUR</uzo>
   <gra><vspec>tr</vspec></gra>
   <snc>
      <dif>
         <tld lit="E"/>i per elektro.
      </dif>
   </snc>
   <adm>
      Kial konservi ligvokalon?? <aut>MB</aut>
   </adm>
   <trd lng="be">&c_p;&c_a;&c_s;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;
   &c_n;&c_a; 
     &c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_y;
     &c_s;&c_t;&c_u;&c_l;</trd>
   <trd lng="ca">electrocutar</trd>
   <trdgrp lng="de">
      <trd>auf dem elektrischen Stuhl hinrichten</trd>
   </trdgrp>
   <trd lng="en">electrocute</trd>
   <trd lng="fr">&eacute;lectrocuter</trd>
   <trd lng="hu">villanysz&eacute;kben kiveacute;gez</trd>
   <trdgrp lng="pl">
     <trd>krzes&lstroke;o elektryczne<klr> (pot.)</klr></trd>
   </trdgrp>
</drv>
</art>
